{
"kv17.3:1.1": "Kathāvatthu",
"kv17.3:2.1": "Mahāpaṇṇāsaka",
"kv17.3:3.1": "Sattarasamavagga",
"kv17.3:4.1": "Sabbamidaṃkammatotikathā",
"kv17.3:5.1": "Sabbamidaṃ kammatoti?",
"kv17.3:5.2": "Āmantā.",
"kv17.3:5.3": "Kammampi kammatoti?",
"kv17.3:5.4": "Na hevaṃ vattabbe … pe …",
"kv17.3:5.6": "sabbamidaṃ kammatoti?",
"kv17.3:5.7": "Āmantā.",
"kv17.3:5.8": "Sabbamidaṃ pubbekatahetūti?",
"kv17.3:5.9": "Na hevaṃ vattabbe … pe …",
"kv17.3:5.11": "sabbamidaṃ kammatoti?",
"kv17.3:5.12": "Āmantā.",
"kv17.3:5.13": "Sabbamidaṃ kammavipākatoti?",
"kv17.3:5.14": "Na hevaṃ vattabbe … pe ….",
"kv17.3:6.1": "Sabbamidaṃ kammavipākatoti?",
"kv17.3:6.2": "Āmantā.",
"kv17.3:6.3": "Kammavipākena pāṇaṃ haneyyāti?",
"kv17.3:6.4": "Āmantā.",
"kv17.3:6.5": "Pāṇātipāto saphaloti?",
"kv17.3:6.6": "Āmantā.",
"kv17.3:6.7": "Kammavipāko saphaloti?",
"kv17.3:6.8": "Na hevaṃ vattabbe … pe …",
"kv17.3:6.10": "kammavipāko aphaloti?",
"kv17.3:6.11": "Āmantā.",
"kv17.3:6.12": "Pāṇātipāto aphaloti?",
"kv17.3:6.13": "Na hevaṃ vattabbe … pe ….",
"kv17.3:7.1": "Kammavipākena adinnaṃ ādiyeyya … pe …",
"kv17.3:7.3": "musā bhaṇeyya …",
"kv17.3:7.4": "pisuṇaṃ bhaṇeyya …",
"kv17.3:7.5": "pharusaṃ bhaṇeyya …",
"kv17.3:7.6": "samphaṃ palapeyya …",
"kv17.3:7.7": "sandhiṃ chindeyya …",
"kv17.3:7.8": "nillopaṃ hareyya …",
"kv17.3:7.9": "ekāgārikaṃ kareyya …",
"kv17.3:7.10": "paripanthe tiṭṭheyya …",
"kv17.3:7.11": "paradāraṃ gaccheyya …",
"kv17.3:7.12": "gāmaghātakaṃ kareyya …",
"kv17.3:7.13": "nigamaghātakaṃ kareyya …",
"kv17.3:7.14": "kammavipākena dānaṃ dadeyya …",
"kv17.3:7.15": "cīvaraṃ dadeyya …",
"kv17.3:7.16": "piṇḍapātaṃ dadeyya …",
"kv17.3:7.17": "senāsanaṃ dadeyya …",
"kv17.3:7.18": "gilānapaccayabhesajjaparikkhāraṃ dadeyyāti?",
"kv17.3:7.19": "Āmantā.",
"kv17.3:7.20": "Gilānapaccayabhesajjaparikkhāro saphaloti?",
"kv17.3:7.21": "Āmantā.",
"kv17.3:7.22": "Kammavipāko saphaloti?",
"kv17.3:7.23": "Na hevaṃ vattabbe … pe …",
"kv17.3:7.25": "kammavipāko aphaloti?",
"kv17.3:7.26": "Āmantā.",
"kv17.3:7.27": "Gilānapaccayabhesajjaparikkhāro aphaloti?",
"kv17.3:7.28": "Na hevaṃ vattabbe … pe ….",
"kv17.3:8.1": "☒ Na vattabbaṃ—",
"kv17.3:8.2": "“sabbamidaṃ kammato”ti?",
"kv17.3:8.3": "Āmantā.",
"kv17.3:8.4": "Nanu vuttaṃ bhagavatā—",
"kv17.3:9.1": "“Kammunā vattatī loko,",
"kv17.3:9.2": "kammunā vattatī pajā;",
"kv17.3:9.3": "Kammanibandhanā sattā,",
"kv17.3:9.4": "rathassāṇīva yāyato.",
"kv17.3:10.1": "Kammena kittiṃ labhate pasaṃsaṃ,",
"kv17.3:10.2": "Kammena jāniñca vadhañca bandhaṃ;",
"kv17.3:10.3": "Taṃ kammaṃ nānākaraṇaṃ viditvā,",
"kv17.3:10.4": "Kasmā vade natthi kammanti loke”ti.",
"kv17.3:11.1": "Attheva suttantoti?",
"kv17.3:11.2": "Āmantā.",
"kv17.3:11.3": "Tena hi “sabbamidaṃ kammato”ti.",
"kv17.3:12.1": "Sabbamidaṃ kammatotikathā niṭṭhitā."
}