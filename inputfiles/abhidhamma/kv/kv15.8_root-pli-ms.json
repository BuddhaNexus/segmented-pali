{
"kv15.8:1.1": "Kathāvatthu",
"kv15.8:2.1": "Mahāpaṇṇāsaka",
"kv15.8:3.1": "Pannarasamavagga",
"kv15.8:4.1": "Dutiyasaññāvedayitakathā",
"kv15.8:5.1": "Saññāvedayitanirodhasamāpatti lokiyāti?",
"kv15.8:5.2": "Āmantā.",
"kv15.8:5.3": "Rūpanti?",
"kv15.8:5.4": "Na hevaṃ vattabbe … pe …",
"kv15.8:5.6": "vedanā …",
"kv15.8:5.7": "saññā …",
"kv15.8:5.8": "saṅkhārā …",
"kv15.8:5.9": "viññāṇanti?",
"kv15.8:5.10": "Na hevaṃ vattabbe … pe …",
"kv15.8:5.12": "kāmāvacarāti?",
"kv15.8:5.13": "Na hevaṃ vattabbe … pe …",
"kv15.8:5.15": "rūpāvacarāti?",
"kv15.8:5.16": "Na hevaṃ vattabbe … pe …",
"kv15.8:5.18": "arūpāvacarāti?",
"kv15.8:5.19": "Na hevaṃ vattabbe … pe ….",
"kv15.8:6.1": "☒ Na vattabbaṃ—",
"kv15.8:6.2": "“saññāvedayitanirodhasamāpatti lokiyā”ti?",
"kv15.8:6.3": "Āmantā.",
"kv15.8:6.4": "Lokuttarāti?",
"kv15.8:6.5": "Na hevaṃ vattabbe.",
"kv15.8:6.6": "Tena hi lokiyāti.",
"kv15.8:7.1": "Dutiyasaññāvedayitakathā niṭṭhitā."
