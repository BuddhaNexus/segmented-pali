{
"kv14.5:1.1": "Kathāvatthu",
"kv14.5:2.1": "Mahāpaṇṇāsaka",
"kv14.5:3.1": "Cuddasamavagga",
"kv14.5:4.1": "Aññoanusayotikathā",
"kv14.5:5.1": "Añño kāmarāgānusayo aññaṃ kāmarāgapariyuṭṭhānanti?",
"kv14.5:5.2": "Āmantā.",
"kv14.5:5.3": "Añño kāmarāgo aññaṃ kāmarāgapariyuṭṭhānanti?",
"kv14.5:5.4": "Na hevaṃ vattabbe … pe …",
"kv14.5:5.6": "sveva kāmarāgo taṃ kāmarāgapariyuṭṭhānanti?",
"kv14.5:5.7": "Āmantā.",
"kv14.5:5.8": "Sveva kāmarāgānusayo taṃ kāmarāgapariyuṭṭhānanti?",
"kv14.5:5.9": "Na hevaṃ vattabbe … pe ….",
"kv14.5:6.1": "Añño paṭighānusayo aññaṃ paṭighapariyuṭṭhānanti?",
"kv14.5:6.2": "Āmantā.",
"kv14.5:6.3": "Aññaṃ paṭighaṃ aññaṃ paṭighapariyuṭṭhānanti?",
"kv14.5:6.4": "Na hevaṃ vattabbe … pe …",
"kv14.5:6.6": "taññeva paṭighaṃ taṃ paṭighapariyuṭṭhānanti?",
"kv14.5:6.7": "Āmantā.",
"kv14.5:6.8": "Sveva paṭighānusayo taṃ paṭighapariyuṭṭhānanti?",
"kv14.5:6.9": "Na hevaṃ vattabbe … pe ….",
"kv14.5:7.1": "Añño mānānusayo aññaṃ mānapariyuṭṭhānanti?",
"kv14.5:7.2": "Āmantā.",
"kv14.5:7.3": "Añño māno aññaṃ mānapariyuṭṭhānanti?",
"kv14.5:7.4": "Na hevaṃ vattabbe … pe …",
"kv14.5:7.6": "sveva māno taṃ mānapariyuṭṭhānanti?",
"kv14.5:7.7": "Āmantā.",
"kv14.5:7.8": "Sveva mānānusayo taṃ mānapariyuṭṭhānanti?",
"kv14.5:7.9": "Na hevaṃ vattabbe … pe ….",
"kv14.5:8.1": "Añño diṭṭhānusayo aññaṃ diṭṭhipariyuṭṭhānanti?",
"kv14.5:8.2": "Āmantā.",
"kv14.5:8.3": "Aññā diṭṭhi aññaṃ diṭṭhipariyuṭṭhānanti?",
"kv14.5:8.4": "Na hevaṃ vattabbe … pe …",
"kv14.5:8.6": "sāva diṭṭhi taṃ diṭṭhipariyuṭṭhānanti?",
"kv14.5:8.7": "Āmantā.",
"kv14.5:8.8": "Sveva diṭṭhānusayo taṃ diṭṭhipariyuṭṭhānanti?",
"kv14.5:8.9": "Na hevaṃ vattabbe … pe ….",
"kv14.5:9.1": "Añño vicikicchānusayo aññaṃ vicikicchāpariyuṭṭhānanti?",
"kv14.5:9.2": "Āmantā.",
"kv14.5:9.3": "Aññā vicikicchā aññaṃ vicikicchāpariyuṭṭhānanti?",
"kv14.5:9.4": "Na hevaṃ vattabbe … pe …",
"kv14.5:9.6": "sāva vicikicchā taṃ vicikicchāpariyuṭṭhānanti?",
"kv14.5:9.7": "Āmantā.",
"kv14.5:9.8": "Sveva vicikicchānusayo taṃ vicikicchāpariyuṭṭhānanti?",
"kv14.5:9.9": "Na hevaṃ vattabbe … pe ….",
"kv14.5:10.1": "Añño bhavarāgānusayo aññaṃ bhavarāgapariyuṭṭhānanti?",
"kv14.5:10.2": "Āmantā.",
"kv14.5:10.3": "Añño bhavarāgo aññaṃ bhavarāgapariyuṭṭhānanti?",
"kv14.5:10.4": "Na hevaṃ vattabbe … pe …",
"kv14.5:10.6": "sveva bhavarāgo taṃ bhavarāgapariyuṭṭhānanti?",
"kv14.5:10.7": "Āmantā.",
"kv14.5:10.8": "Sveva bhavarāgānusayo taṃ bhavarāgapariyuṭṭhānanti?",
"kv14.5:10.9": "Na hevaṃ vattabbe … pe ….",
"kv14.5:11.1": "Añño avijjānusayo aññaṃ avijjāpariyuṭṭhānanti?",
"kv14.5:11.2": "Āmantā.",
"kv14.5:11.3": "Aññā avijjā aññaṃ avijjāpariyuṭṭhānanti?",
"kv14.5:11.4": "Na hevaṃ vattabbe … pe …",
"kv14.5:11.6": "sāva avijjā taṃ avijjāpariyuṭṭhānanti?",
"kv14.5:11.7": "Āmantā.",
"kv14.5:11.8": "Sveva avijjānusayo taṃ avijjāpariyuṭṭhānanti?",
"kv14.5:11.9": "Na hevaṃ vattabbe … pe ….",
"kv14.5:12.1": "☒ Na vattabbaṃ—",
"kv14.5:12.2": "“añño anusayo aññaṃ pariyuṭṭhānan”ti?",
"kv14.5:12.3": "Āmantā.",
"kv14.5:12.4": "Puthujjano kusalābyākate citte vattamāne “sānusayo”ti vattabboti?",
"kv14.5:12.5": "Āmantā.",
"kv14.5:12.6": "“Pariyuṭṭhito”ti vattabboti?",
"kv14.5:12.7": "Na hevaṃ vattabbe.",
"kv14.5:12.8": "Tena hi añño anusayo aññaṃ pariyuṭṭhānanti.",
"kv14.5:12.9": "Puthujjano kusalābyākate citte vattamāne “sarāgo”ti vattabboti?",
"kv14.5:12.10": "Āmantā.",
"kv14.5:12.11": "“Pariyuṭṭhito”ti vattabboti?",
"kv14.5:12.12": "Na hevaṃ vattabbe.",
"kv14.5:12.13": "Tena hi añño rāgo aññaṃ pariyuṭṭhānanti.",
"kv14.5:13.1": "Añño anusayotikathā niṭṭhitā."
}