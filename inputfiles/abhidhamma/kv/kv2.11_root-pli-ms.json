{
"kv2.11:1.1": "Kathāvatthu",
"kv2.11:2.1": "Mahāpaṇṇāsaka",
"kv2.11:3.1": "Dutiyavagga",
"kv2.11:4.1": "Nirodhakathā",
"kv2.11:5.1": "Dve nirodhāti?",
"kv2.11:5.2": "Āmantā.",
"kv2.11:5.3": "Dve dukkhanirodhāti?",
"kv2.11:5.4": "Na hevaṃ vattabbe … pe …",
"kv2.11:5.6": "dve dukkhanirodhāti?",
"kv2.11:5.7": "Āmantā.",
"kv2.11:5.8": "Dve nirodhasaccānīti?",
"kv2.11:5.9": "Na hevaṃ vattabbe … pe …",
"kv2.11:5.11": "dve nirodhasaccānīti?",
"kv2.11:5.12": "Āmantā.",
"kv2.11:5.13": "Dve dukkhasaccānīti?",
"kv2.11:5.14": "Na hevaṃ vattabbe … pe …",
"kv2.11:5.16": "dve nirodhasaccānīti?",
"kv2.11:5.17": "Āmantā.",
"kv2.11:5.18": "Dve samudayasaccānīti?",
"kv2.11:5.19": "Na hevaṃ vattabbe … pe …",
"kv2.11:5.21": "dve nirodhasaccānīti?",
"kv2.11:5.22": "Āmantā.",
"kv2.11:5.23": "Dve maggasaccānīti?",
"kv2.11:5.24": "Na hevaṃ vattabbe … pe ….",
"kv2.11:6.1": "Dve nirodhasaccānīti?",
"kv2.11:6.2": "Āmantā.",
"kv2.11:6.3": "Dve tāṇāni … pe …",
"kv2.11:6.5": "dve leṇāni …",
"kv2.11:6.6": "dve saraṇāni …",
"kv2.11:6.7": "dve parāyaṇāni …",
"kv2.11:6.8": "dve accutāni …",
"kv2.11:6.9": "dve amatāni …",
"kv2.11:6.10": "dve nibbānānīti?",
"kv2.11:6.11": "Na hevaṃ vattabbe … pe ….",
"kv2.11:7.1": "Dve nibbānānīti?",
"kv2.11:7.2": "Āmantā.",
"kv2.11:7.3": "Atthi dvinnaṃ nibbānānaṃ uccanīcatā hīnapaṇītatā ukkaṃsāvakaṃso sīmā vā bhedo vā rāji vā antarikā vāti?",
"kv2.11:7.4": "Na hevaṃ vattabbe … pe ….",
"kv2.11:8.1": "Dve nirodhāti?",
"kv2.11:8.2": "Āmantā.",
"kv2.11:8.3": "Nanu appaṭisaṅkhāniruddhe saṅkhāre paṭisaṅkhā nirodhentīti?",
"kv2.11:8.4": "Āmantā.",
"kv2.11:8.5": "Hañci appaṭisaṅkhāniruddhe saṅkhāre paṭisaṅkhā nirodhenti, no ca vata re vattabbe—",
"kv2.11:8.6": "“dve nirodhā”ti.",
"kv2.11:9.1": "☒ Na vattabbaṃ—",
"kv2.11:9.2": "“dve nirodhā”ti?",
"kv2.11:9.3": "Āmantā.",
"kv2.11:9.4": "Nanu appaṭisaṅkhāniruddhāpi saṅkhārā accantabhaggā, paṭisaṅkhāniruddhāpi saṅkhārā accantabhaggāti?",
"kv2.11:9.5": "Āmantā.",
"kv2.11:9.6": "Hañci appaṭisaṅkhāniruddhāpi saṅkhārā accantabhaggā, paṭisaṅkhāniruddhāpi saṅkhārā accantabhaggā, tena vata re vattabbe—",
"kv2.11:9.7": "“dve nirodhā”ti.",
"kv2.11:10.1": "Dve nirodhāti?",
"kv2.11:10.2": "Āmantā.",
"kv2.11:10.3": "Paṭisaṅkhāniruddhāpi saṅkhārā ariyamaggaṃ āgamma niruddhāti?",
"kv2.11:10.4": "Āmantā.",
"kv2.11:10.5": "Appaṭisaṅkhāniruddhā saṅkhārā ariyamaggaṃ āgamma niruddhāti?",
"kv2.11:10.6": "Na hevaṃ vattabbe … pe …",
"kv2.11:11.1": "Dve nirodhāti?",
"kv2.11:11.2": "Āmantā.",
"kv2.11:11.3": "Paṭisaṅkhāniruddhā saṅkhārā na puna uppajjantīti?",
"kv2.11:11.4": "Āmantā.",
"kv2.11:11.5": "Appaṭisaṅkhāniruddhā saṅkhārā na puna uppajjantīti?",
"kv2.11:11.6": "Na hevaṃ vattabbe … pe …",
"kv2.11:11.8": "tena hi na vattabbaṃ—",
"kv2.11:11.9": "“dve nirodhā”ti.",
"kv2.11:12.1": "Nirodhakathā niṭṭhitā.",
"kv2.11:12.2": "Dutiyo vaggo.",
"kv2.11:13.1": "Tassuddānaṃ",
"kv2.11:14.1": "Parūpahāro aññāṇaṃ,",
"kv2.11:14.2": "kaṅkhā paravitāraṇā;",
"kv2.11:14.3": "Vacībhedo dukkhāhāro,",
"kv2.11:14.4": "cittaṭṭhiti ca kukkuḷā;",
"kv2.11:14.5": "Anupubbābhisamayo,",
"kv2.11:14.6": "vohāro ca nirodhakoti."
}