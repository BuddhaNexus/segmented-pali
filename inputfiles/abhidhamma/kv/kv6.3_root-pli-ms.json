{
"kv6.3:1.1": "Kathāvatthu",
"kv6.3:2.1": "Mahāpaṇṇāsaka",
"kv6.3:3.1": "Chaṭṭhavagga",
"kv6.3:4.1": "Saccakathā",
"kv6.3:5.1": "Cattāri saccāni asaṅkhatānīti?",
"kv6.3:5.2": "Āmantā.",
"kv6.3:5.3": "Cattāri tāṇāni cattāri leṇāni cattāri saraṇāni cattāri parāyanāni cattāri accutāni cattāri amatāni cattāri nibbānānīti?",
"kv6.3:5.4": "Na hevaṃ vattabbe … pe …",
"kv6.3:5.6": "cattāri nibbānānīti?",
"kv6.3:5.7": "Āmantā.",
"kv6.3:5.8": "Atthi catunnaṃ nibbānānaṃ uccanīcatā hīnapaṇītatā ukkaṃsāvakaṃso sīmā vā bhedo vā rāji vā antarikā vāti?",
"kv6.3:5.9": "Na hevaṃ vattabbe … pe ….",
"kv6.3:6.1": "Dukkhasaccaṃ asaṅkhatanti?",
"kv6.3:6.2": "Āmantā.",
"kv6.3:6.3": "Dukkhaṃ asaṅkhatanti?",
"kv6.3:6.4": "Na hevaṃ vattabbe … pe …",
"kv6.3:6.6": "dukkhasaccaṃ asaṅkhatanti?",
"kv6.3:6.7": "Āmantā.",
"kv6.3:6.8": "Kāyikaṃ dukkhaṃ cetasikaṃ dukkhaṃ sokaparidevadukkhadomanassaupāyāsā asaṅkhatāti?",
"kv6.3:6.9": "Na hevaṃ vattabbe … pe …",
"kv6.3:6.11": "samudayasaccaṃ asaṅkhatanti?",
"kv6.3:6.12": "Āmantā.",
"kv6.3:6.13": "Samudayo asaṅkhatoti?",
"kv6.3:6.14": "Na hevaṃ vattabbe … pe …",
"kv6.3:6.16": "samudayasaccaṃ asaṅkhatanti?",
"kv6.3:6.17": "Āmantā.",
"kv6.3:6.18": "Kāmataṇhā bhavataṇhā vibhavataṇhā asaṅkhatāti?",
"kv6.3:6.19": "Na hevaṃ vattabbe … pe …",
"kv6.3:6.21": "maggasaccaṃ asaṅkhatanti?",
"kv6.3:6.22": "Āmantā.",
"kv6.3:6.23": "Maggo asaṅkhatoti?",
"kv6.3:6.24": "Na hevaṃ vattabbe … pe …",
"kv6.3:6.26": "maggasaccaṃ asaṅkhatanti?",
"kv6.3:6.27": "Āmantā.",
"kv6.3:6.28": "sammādiṭṭhi … pe … sammāsamādhi asaṅkhatoti?",
"kv6.3:6.31": "Na hevaṃ vattabbe … pe ….",
"kv6.3:7.1": "Dukkhaṃ saṅkhatanti?",
"kv6.3:7.2": "Āmantā.",
"kv6.3:7.3": "Dukkhasaccaṃ saṅkhatanti?",
"kv6.3:7.4": "Na hevaṃ vattabbe … pe …",
"kv6.3:7.6": "kāyikaṃ dukkhaṃ cetasikaṃ dukkhaṃ sokaparidevadukkhadomanassaupāyāsā saṅkhatāti?",
"kv6.3:7.7": "Āmantā.",
"kv6.3:7.8": "Dukkhasaccaṃ saṅkhatanti?",
"kv6.3:7.9": "Na hevaṃ vattabbe … pe …",
"kv6.3:7.11": "samudayo saṅkhatoti?",
"kv6.3:7.12": "Āmantā.",
"kv6.3:7.13": "Samudayasaccaṃ saṅkhatanti?",
"kv6.3:7.14": "Na hevaṃ vattabbe … pe …",
"kv6.3:7.16": "kāmataṇhā bhavataṇhā vibhavataṇhā saṅkhatāti?",
"kv6.3:7.17": "Āmantā.",
"kv6.3:7.18": "Samudayasaccaṃ saṅkhatanti?",
"kv6.3:7.19": "Na hevaṃ vattabbe … pe …",
"kv6.3:7.21": "maggo saṅkhatoti?",
"kv6.3:7.22": "Āmantā.",
"kv6.3:7.23": "Maggasaccaṃ saṅkhatanti?",
"kv6.3:7.24": "Na hevaṃ vattabbe … pe …",
"kv6.3:7.26": "sammādiṭṭhi … pe … sammāsamādhi saṅkhatoti?",
"kv6.3:7.29": "Āmantā.",
"kv6.3:7.30": "Maggasaccaṃ saṅkhatanti?",
"kv6.3:7.31": "Na hevaṃ vattabbe … pe ….",
"kv6.3:8.1": "Nirodhasaccaṃ asaṅkhataṃ, nirodho asaṅkhatoti?",
"kv6.3:8.2": "Āmantā.",
"kv6.3:8.3": "Dukkhasaccaṃ asaṅkhataṃ, dukkhaṃ asaṅkhatanti?",
"kv6.3:8.4": "Na hevaṃ vattabbe … pe …",
"kv6.3:8.6": "nirodhasaccaṃ asaṅkhataṃ, nirodho asaṅkhatoti?",
"kv6.3:8.7": "Āmantā.",
"kv6.3:8.8": "Samudayasaccaṃ asaṅkhataṃ, samudayo asaṅkhatoti?",
"kv6.3:8.9": "Na hevaṃ vattabbe … pe …",
"kv6.3:8.11": "nirodhasaccaṃ asaṅkhataṃ, nirodho asaṅkhatoti?",
"kv6.3:8.12": "Āmantā.",
"kv6.3:8.13": "Maggasaccaṃ asaṅkhataṃ, maggo asaṅkhatoti?",
"kv6.3:8.14": "Na hevaṃ vattabbe … pe ….",
"kv6.3:9.1": "Dukkhasaccaṃ asaṅkhataṃ, dukkhaṃ saṅkhatanti?",
"kv6.3:9.2": "Āmantā.",
"kv6.3:9.3": "Nirodhasaccaṃ asaṅkhataṃ, nirodho saṅkhatoti?",
"kv6.3:9.4": "Na hevaṃ vattabbe … pe …",
"kv6.3:9.6": "samudayasaccaṃ asaṅkhataṃ, samudayo saṅkhatoti?",
"kv6.3:9.7": "Āmantā.",
"kv6.3:9.8": "Nirodhasaccaṃ asaṅkhataṃ, nirodho saṅkhatoti?",
"kv6.3:9.9": "Na hevaṃ vattabbe … pe …",
"kv6.3:9.11": "maggasaccaṃ asaṅkhataṃ, maggo saṅkhatoti?",
"kv6.3:9.12": "Āmantā.",
"kv6.3:9.13": "Nirodhasaccaṃ asaṅkhataṃ, nirodho saṅkhatoti?",
"kv6.3:9.14": "Na hevaṃ vattabbe … pe ….",
"kv6.3:10.1": "☒ Na vattabbaṃ—",
"kv6.3:10.2": "“cattāri saccāni asaṅkhatānī”ti?",
"kv6.3:10.3": "Āmantā.",
"kv6.3:10.4": "Nanu vuttaṃ bhagavatā—",
"kv6.3:10.5": "“cattārimāni, bhikkhave, tathāni avitathāni anaññathāni.",
"kv6.3:10.6": "Katamāni cattāri?",
"kv6.3:10.7": "‘Idaṃ dukkhan’ti, bhikkhave, tathametaṃ avitathametaṃ anaññathametaṃ … pe …",
"kv6.3:10.9": "‘ayaṃ dukkhasamudayo’ti … pe …",
"kv6.3:10.11": "‘ayaṃ dukkhanirodho’ti … pe …",
"kv6.3:10.13": "‘ayaṃ dukkhanirodhagāminī paṭipadā’ti tathametaṃ avitathametaṃ anaññathametaṃ.",
"kv6.3:10.14": "Imāni kho, bhikkhave, cattāri tathāni avitathāni anaññathānī”ti.",
"kv6.3:10.15": "Attheva suttantoti?",
"kv6.3:10.16": "Āmantā.",
"kv6.3:10.17": "Tena hi cattāri saccāni asaṅkhatānīti.",
"kv6.3:11.1": "Saccakathā niṭṭhitā."
}