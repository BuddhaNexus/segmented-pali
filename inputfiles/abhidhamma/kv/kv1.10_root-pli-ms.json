{
"kv1.10:1.1": "Kathāvatthu",
"kv1.10:2.1": "Mahāpaṇṇāsaka",
"kv1.10:3.1": "Paṭhamavagga",
"kv1.10:4.1": "Hevatthikathā",
"kv1.10:5.1": "Atītaṃ atthīti?",
"kv1.10:5.2": "Hevatthi, heva natthīti.",
"kv1.10:5.3": "Sevatthi, seva natthīti?",
"kv1.10:5.4": "Na hevaṃ vattabbe … pe …",
"kv1.10:5.6": "sevatthi, seva natthīti?",
"kv1.10:5.7": "Āmantā.",
"kv1.10:5.8": "Atthaṭṭho natthaṭṭho, natthaṭṭho atthaṭṭho, atthibhāvo natthibhāvo, natthibhāvo atthibhāvo, atthīti vā natthīti vā, natthīti vā atthīti vā esese ekaṭṭhe same samabhāge tajjāteti?",
"kv1.10:5.9": "Na hevaṃ vattabbe … pe ….",
"kv1.10:6.1": "Anāgataṃ atthīti?",
"kv1.10:6.2": "Hevatthi, heva natthīti.",
"kv1.10:6.3": "Sevatthi, seva natthīti?",
"kv1.10:6.4": "Na hevaṃ vattabbe … pe …",
"kv1.10:6.6": "sevatthi, seva natthīti?",
"kv1.10:6.7": "Āmantā.",
"kv1.10:6.8": "Atthaṭṭho natthaṭṭho, natthaṭṭho atthaṭṭho, atthibhāvo natthibhāvo, natthibhāvo atthibhāvo, atthīti vā natthīti vā, natthīti vā atthīti vā esese ekaṭṭhe same samabhāge tajjāteti?",
"kv1.10:6.9": "Na hevaṃ vattabbe … pe ….",
"kv1.10:7.1": "Paccuppannaṃ atthīti?",
"kv1.10:7.2": "Hevatthi, heva natthīti.",
"kv1.10:7.3": "Sevatthi, seva natthīti?",
"kv1.10:7.4": "Na hevaṃ vattabbe … pe …",
"kv1.10:7.6": "sevatthi, seva natthīti?",
"kv1.10:7.7": "Āmantā.",
"kv1.10:7.8": "Atthaṭṭho natthaṭṭho … pe …",
"kv1.10:7.10": "same samabhāge tajjāteti?",
"kv1.10:7.11": "Na hevaṃ vattabbe … pe ….",
"kv1.10:8.1": "Atītaṃ hevatthi, heva natthīti?",
"kv1.10:8.2": "Āmantā.",
"kv1.10:8.3": "Kintatthi, kinti natthīti?",
"kv1.10:8.4": "Atītaṃ atītanti hevatthi, atītaṃ anāgatanti heva natthi, atītaṃ paccuppannanti heva natthīti.",
"kv1.10:8.5": "Sevatthi, seva natthīti?",
"kv1.10:8.6": "Na hevaṃ vattabbe … pe …",
"kv1.10:8.8": "sevatthi, seva natthīti?",
"kv1.10:8.9": "Āmantā.",
"kv1.10:8.10": "Atthaṭṭho natthaṭṭho, natthaṭṭho atthaṭṭho, atthibhāvo natthibhāvo, natthibhāvo atthibhāvo, atthīti vā natthīti vā, natthīti vā atthīti vā esese ekaṭṭhe same samabhāge tajjāteti?",
"kv1.10:8.11": "Na hevaṃ vattabbe … pe ….",
"kv1.10:9.1": "Anāgataṃ hevatthi, heva natthīti?",
"kv1.10:9.2": "Āmantā.",
"kv1.10:9.3": "Kintatthi, kinti natthīti?",
"kv1.10:9.4": "Anāgataṃ anāgatanti hevatthi, anāgataṃ atītanti heva natthi, anāgataṃ paccuppannanti heva natthīti.",
"kv1.10:9.5": "Sevatthi, seva natthīti?",
"kv1.10:9.6": "Na hevaṃ vattabbe … pe …",
"kv1.10:9.8": "sevatthi, seva natthīti?",
"kv1.10:9.9": "Āmantā.",
"kv1.10:9.10": "Atthaṭṭho natthaṭṭho, natthaṭṭho atthaṭṭho … pe …",
"kv1.10:9.12": "same samabhāge tajjāteti?",
"kv1.10:9.13": "Na hevaṃ vattabbe … pe ….",
"kv1.10:10.1": "Paccuppannaṃ hevatthi, heva natthīti?",
"kv1.10:10.2": "Āmantā.",
"kv1.10:10.3": "Kintatthi, kinti natthīti?",
"kv1.10:10.4": "Paccuppannaṃ paccuppannanti hevatthi, paccuppannaṃ atītanti heva natthi, paccuppannaṃ anāgatanti heva natthīti.",
"kv1.10:10.5": "Sevatthi, seva natthīti?",
"kv1.10:10.6": "Na hevaṃ vattabbe … pe …",
"kv1.10:10.8": "sevatthi, seva natthīti?",
"kv1.10:10.9": "Āmantā.",
"kv1.10:10.10": "Atthaṭṭho natthaṭṭho … pe …",
"kv1.10:10.12": "same samabhāge tajjāteti?",
"kv1.10:10.13": "Na hevaṃ vattabbe … pe ….",
"kv1.10:11.1": "☒ Na vattabbaṃ—",
"kv1.10:11.2": "“atītaṃ hevatthi, heva natthi;",
"kv1.10:11.3": "anāgataṃ hevatthi, heva natthi;",
"kv1.10:11.4": "paccuppannaṃ hevatthi, heva natthī”ti?",
"kv1.10:11.5": "Āmantā.",
"kv1.10:11.6": "Atītaṃ anāgatanti hevatthi, atītaṃ paccuppannanti hevatthi, anāgataṃ atītanti hevatthi, anāgataṃ paccuppannanti hevatthi, paccuppannaṃ atītanti hevatthi, paccuppannaṃ anāgatanti hevatthīti?",
"kv1.10:11.7": "Na hevaṃ vattabbe. … pe ….",
"kv1.10:11.10": "Tena hi atītaṃ hevatthi heva natthi, anāgataṃ hevatthi heva natthi, paccuppannaṃ hevatthi, heva natthīti.",
"kv1.10:12.1": "Rūpaṃ atthīti?",
"kv1.10:12.2": "Hevatthi, heva natthīti.",
"kv1.10:12.3": "Sevatthi, seva natthīti?",
"kv1.10:12.4": "Na hevaṃ vattabbe … pe …",
"kv1.10:12.6": "sevatthi, seva natthīti?",
"kv1.10:12.7": "Āmantā.",
"kv1.10:12.8": "Atthaṭṭho natthaṭṭho, natthaṭṭho atthaṭṭho, atthibhāvo natthibhāvo, natthibhāvo atthibhāvo, atthīti vā natthīti vā, natthīti vā atthīti vā esese ekaṭṭhe same samabhāge tajjāteti?",
"kv1.10:12.9": "Na hevaṃ vattabbe … pe ….",
"kv1.10:13.1": "Vedanā …",
"kv1.10:13.2": "saññā …",
"kv1.10:13.3": "saṅkhārā …",
"kv1.10:13.4": "viññāṇaṃ atthīti?",
"kv1.10:13.5": "Hevatthi, heva natthīti.",
"kv1.10:13.6": "Sevatthi, seva natthīti?",
"kv1.10:13.7": "Na hevaṃ vattabbe … pe …",
"kv1.10:13.9": "sevatthi, seva natthīti?",
"kv1.10:13.10": "Āmantā.",
"kv1.10:13.11": "Atthaṭṭho natthaṭṭho … pe …",
"kv1.10:13.13": "same samabhāge tajjāteti?",
"kv1.10:13.14": "Na hevaṃ vattabbe … pe ….",
"kv1.10:14.1": "Rūpaṃ hevatthi, heva natthīti?",
"kv1.10:14.2": "Āmantā.",
"kv1.10:14.3": "Kintatthi, kinti natthīti?",
"kv1.10:14.4": "Rūpaṃ rūpanti hevatthi, rūpaṃ vedanāti heva natthi … pe …",
"kv1.10:14.6": "rūpaṃ saññāti heva natthi … pe …",
"kv1.10:14.8": "rūpaṃ saṅkhārāti heva natthi … pe …",
"kv1.10:14.10": "rūpaṃ viññāṇanti heva natthīti.",
"kv1.10:14.11": "Sevatthi, seva natthīti?",
"kv1.10:14.12": "Na hevaṃ vattabbe … pe …",
"kv1.10:14.14": "sevatthi, seva natthīti?",
"kv1.10:14.15": "Āmantā.",
"kv1.10:14.16": "Atthaṭṭho natthaṭṭho … pe …",
"kv1.10:14.18": "same samabhāge tajjāteti?",
"kv1.10:14.19": "Na hevaṃ vattabbe … pe ….",
"kv1.10:15.1": "Vedanā …",
"kv1.10:15.2": "saññā …",
"kv1.10:15.3": "saṅkhārā …",
"kv1.10:15.4": "viññāṇaṃ hevatthi, heva natthīti?",
"kv1.10:15.5": "Āmantā.",
"kv1.10:15.6": "Kintatthi, kinti natthīti?",
"kv1.10:15.7": "Viññāṇaṃ viññāṇanti hevatthi.",
"kv1.10:15.8": "Viññāṇaṃ rūpanti heva natthi … pe …",
"kv1.10:15.10": "viññāṇaṃ vedanāti heva natthi … pe …",
"kv1.10:15.12": "viññāṇaṃ saññāti heva natthi … pe …",
"kv1.10:15.14": "viññāṇaṃ saṅkhārāti heva natthīti.",
"kv1.10:15.15": "Sevatthi, seva natthīti?",
"kv1.10:15.16": "Na hevaṃ vattabbe … pe …",
"kv1.10:15.18": "sevatthi, seva natthīti?",
"kv1.10:15.19": "Āmantā.",
"kv1.10:15.20": "Atthaṭṭho natthaṭṭho … pe …",
"kv1.10:15.22": "same samabhāge tajjāteti?",
"kv1.10:15.23": "Na hevaṃ vattabbe … pe ….",
"kv1.10:16.1": "☒ Na vattabbaṃ—",
"kv1.10:16.2": "“rūpaṃ hevatthi, heva natthīti;",
"kv1.10:16.3": "vedanā …",
"kv1.10:16.4": "saññā …",
"kv1.10:16.5": "saṅkhārā …",
"kv1.10:16.6": "viññāṇaṃ hevatthi, heva natthīti?",
"kv1.10:16.7": "Āmantā.",
"kv1.10:16.8": "Rūpaṃ vedanāti hevatthi … pe …",
"kv1.10:16.10": "rūpaṃ saññāti hevatthi … pe …",
"kv1.10:16.12": "rūpaṃ saṅkhārāti hevatthi … pe …",
"kv1.10:16.14": "rūpaṃ viññāṇanti hevatthi …",
"kv1.10:16.15": "vedanā …",
"kv1.10:16.16": "saññā …",
"kv1.10:16.17": "saṅkhārā …",
"kv1.10:16.18": "viññāṇaṃ rūpanti hevatthi …",
"kv1.10:16.19": "viññāṇaṃ vedanāti hevatthi …",
"kv1.10:16.20": "viññāṇaṃ saññāti hevatthi …",
"kv1.10:16.21": "viññāṇaṃ saṅkhārāti hevatthīti?",
"kv1.10:16.22": "Na hevaṃ vattabbe … pe …",
"kv1.10:16.24": "tena hi rūpaṃ hevatthi, heva natthi;",
"kv1.10:16.25": "vedanā …",
"kv1.10:16.26": "saññā …",
"kv1.10:16.27": "saṅkhārā …",
"kv1.10:16.28": "viññāṇaṃ hevatthi, heva natthīti.",
"kv1.10:17.1": "Hevatthikathā niṭṭhitā.",
"kv1.10:18.1": "Tassuddānaṃ",
"kv1.10:19.1": "Upalabbho parihāni,",
"kv1.10:19.2": "Brahmacariyavāso odhiso;",
"kv1.10:19.3": "Pariññā kāmarāgappahānaṃ,",
"kv1.10:19.4": "Sabbatthivādo āyatanaṃ;",
"kv1.10:19.5": "Atītānāgato subhaṅgo,",
"kv1.10:19.6": "Sabbe dhammā satipaṭṭhānā;",
"kv1.10:20.1": "Hevatthi heva natthīti.",
"kv1.10:20.2": "Mahāvaggo."
