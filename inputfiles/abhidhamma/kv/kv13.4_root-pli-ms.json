{
"kv13.4:1.1": "Kathāvatthu",
"kv13.4:2.1": "Mahāpaṇṇāsaka",
"kv13.4:3.1": "Terasamavagga",
"kv13.4:4.1": "Niyatassaniyāmakathā",
"kv13.4:5.1": "Niyato niyāmaṃ okkamatīti?",
"kv13.4:5.2": "Āmantā.",
"kv13.4:5.3": "Micchattaniyato sammattaniyāmaṃ okkamati, sammattaniyato micchattaniyāmaṃ okkamatīti?",
"kv13.4:5.4": "Na hevaṃ vattabbe … pe ….",
"kv13.4:6.1": "Niyato niyāmaṃ okkamatīti?",
"kv13.4:6.2": "Āmantā.",
"kv13.4:6.3": "Pubbe maggaṃ bhāvetvā pacchā niyāmaṃ okkamatīti?",
"kv13.4:6.4": "Na hevaṃ vattabbe … pe …",
"kv13.4:6.6": "pubbe sotāpattimaggaṃ bhāvetvā pacchā sotāpattiniyāmaṃ okkamatīti?",
"kv13.4:6.7": "Na hevaṃ vattabbe … pe …",
"kv13.4:6.9": "pubbe sakadāgāmi … pe …",
"kv13.4:6.11": "anāgāmi … pe …",
"kv13.4:6.13": "arahattamaggaṃ bhāvetvā pacchā arahattaniyāmaṃ okkamatīti?",
"kv13.4:6.14": "Na hevaṃ vattabbe … pe ….",
"kv13.4:7.1": "Pubbe satipaṭṭhānaṃ … pe …",
"kv13.4:7.3": "sammappadhānaṃ …",
"kv13.4:7.4": "iddhipādaṃ …",
"kv13.4:7.5": "indriyaṃ …",
"kv13.4:7.6": "balaṃ …",
"kv13.4:7.7": "bojjhaṅgaṃ bhāvetvā pacchā niyāmaṃ okkamatīti?",
"kv13.4:7.8": "Na hevaṃ vattabbe … pe ….",
"kv13.4:8.1": "☒ Na vattabbaṃ—",
"kv13.4:8.2": "“niyato niyāmaṃ okkamatī”ti?",
"kv13.4:8.3": "Āmantā.",
"kv13.4:8.4": "Bhabbo bodhisatto tāya jātiyā dhammaṃ nābhisametunti?",
"kv13.4:8.5": "Na hevaṃ vattabbe.",
"kv13.4:8.6": "Tena hi niyato niyāmaṃ okkamatīti.",
"kv13.4:9.1": "Niyatassa niyāmakathā niṭṭhitā."
}