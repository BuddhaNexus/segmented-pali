{
"kv3.8:1.1": "Kathāvatthu",
"kv3.8:2.1": "Mahāpaṇṇāsaka",
"kv3.8:3.1": "Tatiyavagga",
"kv3.8:4.1": "Dibbasotakathā",
"kv3.8:5.1": "Maṃsasotaṃ dhammupatthaddhaṃ dibbasotaṃ hotīti?",
"kv3.8:5.2": "Āmantā.",
"kv3.8:5.3": "Maṃsasotaṃ dibbasotaṃ, dibbasotaṃ maṃsasotanti?",
"kv3.8:5.4": "Na hevaṃ vattabbe … pe ….",
"kv3.8:6.1": "Maṃsasotaṃ dhammupatthaddhaṃ dibbasotaṃ hotīti?",
"kv3.8:6.2": "Āmantā.",
"kv3.8:6.3": "Yādisaṃ maṃsasotaṃ tādisaṃ dibbasotaṃ, yādisaṃ dibbasotaṃ tādisaṃ maṃsasotanti?",
"kv3.8:6.4": "Na hevaṃ vattabbe … pe ….",
"kv3.8:7.1": "Maṃsasotaṃ dhammupatthaddhaṃ dibbasotaṃ hotīti?",
"kv3.8:7.2": "Āmantā.",
"kv3.8:7.3": "Taññeva maṃsasotaṃ taṃ dibbasotaṃ, taṃ dibbasotaṃ taṃ maṃsasotanti?",
"kv3.8:7.4": "Na hevaṃ vattabbe … pe ….",
"kv3.8:8.1": "Maṃsasotaṃ dhammupatthaddhaṃ dibbasotaṃ hotīti?",
"kv3.8:8.2": "Āmantā.",
"kv3.8:8.3": "Yādiso maṃsasotassa visayo ānubhāvo gocaro tādiso dibbassa sotassa visayo ānubhāvo gocaroti?",
"kv3.8:8.4": "Na hevaṃ vattabbe … pe ….",
"kv3.8:9.1": "Maṃsasotaṃ dhammupatthaddhaṃ dibbasotaṃ hotīti?",
"kv3.8:9.2": "Āmantā.",
"kv3.8:9.3": "Upādinnaṃ hutvā anupādinnaṃ hotīti?",
"kv3.8:9.4": "Na hevaṃ vattabbe … pe ….",
"kv3.8:10.1": "Upādinnaṃ hutvā anupādinnaṃ hotīti?",
"kv3.8:10.2": "Āmantā.",
"kv3.8:10.3": "Kāmāvacaraṃ hutvā rūpāvacaraṃ hotīti?",
"kv3.8:10.4": "Na hevaṃ vattabbe … pe ….",
"kv3.8:11.1": "Kāmāvacaraṃ hutvā rūpāvacaraṃ hotīti?",
"kv3.8:11.2": "Āmantā.",
"kv3.8:11.3": "Rūpāvacaraṃ hutvā arūpāvacaraṃ hotīti?",
"kv3.8:11.4": "Na hevaṃ vattabbe … pe ….",
"kv3.8:12.1": "Rūpāvacaraṃ hutvā arūpāvacaraṃ hotīti?",
"kv3.8:12.2": "Āmantā.",
"kv3.8:12.3": "Pariyāpannaṃ hutvā apariyāpannaṃ hotīti?",
"kv3.8:12.4": "Na hevaṃ vattabbe … pe ….",
"kv3.8:13.1": "Maṃsasotaṃ dhammupatthaddhaṃ dibbasotaṃ hotīti?",
"kv3.8:13.2": "Āmantā.",
"kv3.8:13.3": "Dibbasotaṃ dhammupatthaddhaṃ maṃsasotaṃ hotīti?",
"kv3.8:13.4": "Na hevaṃ vattabbe … pe ….",
"kv3.8:14.1": "Maṃsasotaṃ dhammupatthaddhaṃ dibbasotaṃ hotīti?",
"kv3.8:14.2": "Āmantā.",
"kv3.8:14.3": "Ekaṃyeva sotanti?",
"kv3.8:14.4": "Na hevaṃ vattabbe … pe ….",
"kv3.8:15.1": "Ekaṃyeva sotanti?",
"kv3.8:15.2": "Āmantā.",
"kv3.8:15.3": "Nanu dve sotāni vuttāni bhagavatā—",
"kv3.8:15.4": "“maṃsasotaṃ, dibbasotan”ti?",
"kv3.8:15.5": "Āmantā.",
"kv3.8:15.6": "Hañci dve sotāni vuttāni bhagavatā—",
"kv3.8:15.7": "maṃsasotaṃ, dibbasotaṃ, no ca vata re vattabbe—",
"kv3.8:15.8": "“ekaññeva sotan”ti.",
"kv3.8:16.1": "Dibbasotakathā niṭṭhitā."
}