{
"kv12.2:1.1": "Kathāvatthu",
"kv12.2:2.1": "Mahāpaṇṇāsaka",
"kv12.2:3.1": "Dvādasamavagga",
"kv12.2:4.1": "Kammakathā",
"kv12.2:5.1": "Sabbaṃ kammaṃ savipākanti?",
"kv12.2:5.2": "Āmantā.",
"kv12.2:5.3": "Sabbā cetanā savipākāti?",
"kv12.2:5.4": "Na hevaṃ vattabbe … pe …",
"kv12.2:5.6": "sabbā cetanā savipākāti?",
"kv12.2:5.7": "Āmantā.",
"kv12.2:5.8": "Vipākābyākatā cetanā savipākāti?",
"kv12.2:5.9": "Na hevaṃ vattabbe … pe …",
"kv12.2:5.11": "sabbā cetanā savipākāti?",
"kv12.2:5.12": "Āmantā.",
"kv12.2:5.13": "Kiriyābyākatā cetanā savipākāti?",
"kv12.2:5.14": "Na hevaṃ vattabbe … pe ….",
"kv12.2:6.1": "Sabbā cetanā savipākāti?",
"kv12.2:6.2": "Āmantā.",
"kv12.2:6.3": "Kāmāvacarā vipākābyākatā cetanā savipākāti?",
"kv12.2:6.4": "Na hevaṃ vattabbe … pe …",
"kv12.2:6.6": "sabbā cetanā savipākāti?",
"kv12.2:6.7": "Āmantā.",
"kv12.2:6.8": "Rūpāvacarā arūpāvacarā apariyāpannā vipākābyākatā cetanā savipākāti?",
"kv12.2:6.9": "Na hevaṃ vattabbe … pe ….",
"kv12.2:7.1": "Sabbā cetanā savipākāti?",
"kv12.2:7.2": "Āmantā.",
"kv12.2:7.3": "Kāmāvacarā kiriyābyākatā cetanā savipākāti?",
"kv12.2:7.4": "Na hevaṃ vattabbe … pe …",
"kv12.2:7.6": "sabbā cetanā savipākāti?",
"kv12.2:7.7": "Āmantā.",
"kv12.2:7.8": "Rūpāvacarā arūpāvacarā kiriyābyākatā cetanā savipākāti?",
"kv12.2:7.9": "Na hevaṃ vattabbe … pe ….",
"kv12.2:8.1": "Vipākābyākatā cetanā avipākāti?",
"kv12.2:8.2": "Āmantā.",
"kv12.2:8.3": "Hañci vipākābyākatā cetanā avipākā, no ca vata re vattabbe—",
"kv12.2:8.4": "“sabbā cetanā savipākā”ti.",
"kv12.2:9.1": "Kiriyābyākatā cetanā avipākāti?",
"kv12.2:9.2": "Āmantā.",
"kv12.2:9.3": "Hañci kiriyābyākatā cetanā avipākā, no ca vata re vattabbe—",
"kv12.2:9.4": "“sabbā cetanā savipākā”ti.",
"kv12.2:10.1": "Kāmāvacarā rūpāvacarā arūpāvacarā apariyāpannā vipākābyākatā cetanā avipākāti?",
"kv12.2:10.2": "Āmantā.",
"kv12.2:10.3": "Hañci apariyāpannā vipākābyākatā cetanā avipākā, no ca vata re vattabbe—",
"kv12.2:10.4": "“sabbā cetanā savipākā”ti.",
"kv12.2:11.1": "Kāmāvacarā rūpāvacarā arūpāvacarā kiriyābyākatā cetanā avipākāti?",
"kv12.2:11.2": "Āmantā.",
"kv12.2:11.3": "Hañci arūpāvacarā kiriyābyākatā cetanā avipākā, no ca vata re vattabbe—",
"kv12.2:11.4": "“sabbā cetanā savipākā”ti.",
"kv12.2:12.1": "☒ Na vattabbaṃ—",
"kv12.2:12.2": "“sabbaṃ kammaṃ savipākan”ti?",
"kv12.2:12.3": "Āmantā.",
"kv12.2:12.4": "Nanu vuttaṃ bhagavatā—",
"kv12.2:12.5": "“nāhaṃ, bhikkhave, sañcetanikānaṃ kammānaṃ katānaṃ upacitānaṃ appaṭisaṃviditvā byantibhāvaṃ vadāmi, tañca kho diṭṭheva dhamme upapajje vā apare vā pariyāye”ti.",
"kv12.2:12.6": "Attheva suttantoti?",
"kv12.2:12.7": "Āmantā.",
"kv12.2:12.8": "Tena hi sabbaṃ kammaṃ savipākanti.",
"kv12.2:13.1": "Kammakathā niṭṭhitā."
}