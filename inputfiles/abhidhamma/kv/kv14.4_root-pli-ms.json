{
"kv14.4:1.1": "Kathāvatthu",
"kv14.4:2.1": "Mahāpaṇṇāsaka",
"kv14.4:3.1": "Cuddasamavagga",
"kv14.4:4.1": "Ariyarūpakathā",
"kv14.4:5.1": "Ariyarūpaṃ mahābhūtānaṃ upādāyāti?",
"kv14.4:5.2": "Āmantā.",
"kv14.4:5.3": "Ariyarūpaṃ kusalanti?",
"kv14.4:5.4": "Āmantā.",
"kv14.4:5.5": "Mahābhūtā kusalāti?",
"kv14.4:5.6": "Na hevaṃ vattabbe … pe …",
"kv14.4:5.8": "mahābhūtā abyākatāti?",
"kv14.4:5.9": "Āmantā.",
"kv14.4:5.10": "Ariyarūpaṃ abyākatanti?",
"kv14.4:5.11": "Na hevaṃ vattabbe … pe …",
"kv14.4:5.13": "ariyarūpaṃ mahābhūtānaṃ upādāyāti?",
"kv14.4:5.14": "Āmantā.",
"kv14.4:5.15": "Ariyarūpaṃ anāsavaṃ asaṃyojaniyaṃ aganthaniyaṃ anoghaniyaṃ ayoganiyaṃ anīvaraṇiyaṃ aparāmaṭṭhaṃ anupādāniyaṃ asaṃkilesiyanti?",
"kv14.4:5.16": "Āmantā.",
"kv14.4:5.17": "Mahābhūtā anāsavā … pe … asaṃkilesiyāti?",
"kv14.4:5.20": "Na hevaṃ vattabbe … pe …",
"kv14.4:5.22": "mahābhūtā sāsavā saṃyojaniyā … pe … saṃkilesiyāti?",
"kv14.4:5.25": "Āmantā.",
"kv14.4:5.26": "Ariyarūpaṃ sāsavaṃ saṃyojaniyaṃ … pe … saṃkilesiyanti?",
"kv14.4:5.29": "Na hevaṃ vattabbe … pe ….",
"kv14.4:6.1": "☒ Na vattabbaṃ—",
"kv14.4:6.2": "“ariyarūpaṃ mahābhūtānaṃ upādāyā”ti?",
"kv14.4:6.3": "Āmantā.",
"kv14.4:6.4": "Nanu vuttaṃ bhagavatā—",
"kv14.4:6.5": "“yaṃ kiñci, bhikkhave, rūpaṃ cattāri mahābhūtāni catunnañca mahābhūtānaṃ upādāyarūpan”ti.",
"kv14.4:6.6": "Attheva suttantoti?",
"kv14.4:6.7": "Āmantā.",
"kv14.4:6.8": "Tena hi ariyarūpaṃ mahābhūtānaṃ upādāyāti.",
"kv14.4:7.1": "Ariyarūpakathā niṭṭhitā."
}