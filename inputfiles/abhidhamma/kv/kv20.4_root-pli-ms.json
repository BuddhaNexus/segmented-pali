{
"kv20.4:1.1": "Kathāvatthu",
"kv20.4:2.1": "Mahāpaṇṇāsaka",
"kv20.4:3.1": "Vīsatimavagga",
"kv20.4:4.1": "Tiracchānakathā",
"kv20.4:5.1": "Atthi devesu tiracchānagatāti?",
"kv20.4:5.2": "Āmantā.",
"kv20.4:5.3": "Atthi tiracchānagatesu devāti?",
"kv20.4:5.4": "Na hevaṃ vattabbe … pe …",
"kv20.4:5.6": "atthi devesu tiracchānagatāti?",
"kv20.4:5.7": "Āmantā.",
"kv20.4:5.8": "Devaloko tiracchānayonīti?",
"kv20.4:5.9": "Na hevaṃ vattabbe … pe …",
"kv20.4:5.11": "atthi devesu tiracchānagatāti?",
"kv20.4:5.12": "Āmantā.",
"kv20.4:5.13": "Atthi tattha kīṭā paṭaṅgā makasā makkhikā ahī vicchikā satapadī gaṇḍuppādāti?",
"kv20.4:5.14": "Na hevaṃ vattabbe … pe ….",
"kv20.4:6.1": "☒ Natthi devesu tiracchānagatāti?",
"kv20.4:6.2": "Āmantā.",
"kv20.4:6.3": "Nanu atthi tattha erāvaṇo nāma hatthināgo sahassayuttaṃ dibbaṃ yānanti?",
"kv20.4:6.4": "Āmantā.",
"kv20.4:6.5": "Hañci atthi tattha erāvaṇo nāma hatthināgo sahassayuttaṃ dibbaṃ yānaṃ, tena vata re vattabbe—",
"kv20.4:6.6": "“atthi devesu tiracchānagatā”ti.",
"kv20.4:7.1": "Atthi devesu tiracchānagatāti?",
"kv20.4:7.2": "Āmantā.",
"kv20.4:7.3": "Atthi tattha hatthibandhā assabandhā yāvasikā kāraṇikā bhattakārakāti?",
"kv20.4:7.4": "Na hevaṃ vattabbe.",
"kv20.4:7.5": "Tena hi natthi devesu tiracchānagatāti.",
"kv20.4:8.1": "Tiracchānakathā niṭṭhitā."
}