{
"kv16.4:1.1": "Kathāvatthu",
"kv16.4:2.1": "Mahāpaṇṇāsaka",
"kv16.4:3.1": "Soḷasamavagga",
"kv16.4:4.1": "Adhigayhamanasikārakathā",
"kv16.4:5.1": "Adhigayha manasi karotīti?",
"kv16.4:5.2": "Āmantā.",
"kv16.4:5.3": "Tena cittena taṃ cittaṃ pajānātīti?",
"kv16.4:5.4": "Na hevaṃ vattabbe … pe …",
"kv16.4:5.6": "tena cittena taṃ cittaṃ pajānātīti?",
"kv16.4:5.7": "Āmantā.",
"kv16.4:5.8": "Tena cittena taṃ cittaṃ “cittan”ti pajānātīti?",
"kv16.4:5.9": "Na hevaṃ vattabbe … pe …",
"kv16.4:5.11": "tena cittena taṃ cittaṃ “cittan”ti pajānātīti?",
"kv16.4:5.12": "Āmantā.",
"kv16.4:5.13": "Taṃ cittaṃ tassa cittassa ārammaṇanti?",
"kv16.4:5.14": "Na hevaṃ vattabbe … pe ….",
"kv16.4:6.1": "Taṃ cittaṃ tassa cittassa ārammaṇanti?",
"kv16.4:6.2": "Āmantā.",
"kv16.4:6.3": "Tena phassena taṃ phassaṃ phusati, tāya vedanāya … pe …",
"kv16.4:6.5": "tāya saññāya …",
"kv16.4:6.6": "tāya cetanāya …",
"kv16.4:6.7": "tena cittena …",
"kv16.4:6.8": "tena vitakkena …",
"kv16.4:6.9": "tena vicārena …",
"kv16.4:6.10": "tāya pītiyā …",
"kv16.4:6.11": "tāya satiyā …",
"kv16.4:6.12": "tāya paññāya taṃ paññaṃ pajānātīti?",
"kv16.4:6.13": "Na hevaṃ vattabbe … pe ….",
"kv16.4:7.1": "Atītaṃ “atītan”ti manasikaronto, anāgataṃ “anāgatan”ti manasi karotīti?",
"kv16.4:7.2": "Na hevaṃ vattabbe … pe …",
"kv16.4:7.4": "atītaṃ “atītan”ti manasikaronto, anāgataṃ “anāgatan”ti manasi karotīti?",
"kv16.4:7.5": "Āmantā.",
"kv16.4:7.6": "Dvinnaṃ phassānaṃ … pe …",
"kv16.4:7.8": "dvinnaṃ cittānaṃ samodhānaṃ hotīti?",
"kv16.4:7.9": "Na hevaṃ vattabbe … pe ….",
"kv16.4:8.1": "Atītaṃ “atītan”ti manasikaronto, paccuppannaṃ “paccuppannan”ti manasi karotīti?",
"kv16.4:8.2": "Na hevaṃ vattabbe … pe …",
"kv16.4:8.4": "atītaṃ “atītan”ti manasikaronto, paccuppannaṃ “paccuppannan”ti manasi karotīti?",
"kv16.4:8.5": "Āmantā.",
"kv16.4:8.6": "Dvinnaṃ phassānaṃ … pe …",
"kv16.4:8.8": "dvinnaṃ cittānaṃ samodhānaṃ hotīti?",
"kv16.4:8.9": "Na hevaṃ vattabbe … pe ….",
"kv16.4:9.1": "Atītaṃ “atītan”ti manasikaronto, anāgataṃ “anāgatan”ti manasi karoti, paccuppannaṃ “paccuppannan”ti manasi karotīti?",
"kv16.4:9.2": "Na hevaṃ vattabbe … pe …",
"kv16.4:9.4": "atītaṃ “atītan”ti manasikaronto, anāgataṃ “anāgatan”ti manasi karoti, paccuppannaṃ “paccuppannan”ti manasi karotīti?",
"kv16.4:9.5": "Āmantā.",
"kv16.4:9.6": "Tiṇṇaṃ phassānaṃ … pe …",
"kv16.4:9.8": "tiṇṇaṃ cittānaṃ samodhānaṃ hotīti?",
"kv16.4:9.9": "Na hevaṃ vattabbe … pe ….",
"kv16.4:10.1": "Anāgataṃ “anāgatan”ti manasikaronto, atītaṃ “atītan”ti manasi karotīti?",
"kv16.4:10.2": "Na hevaṃ vattabbe … pe …",
"kv16.4:10.4": "anāgataṃ “anāgatan”ti manasikaronto, atītaṃ “atītan”ti manasi karotīti?",
"kv16.4:10.5": "Āmantā.",
"kv16.4:10.6": "Dvinnaṃ phassānaṃ … pe …",
"kv16.4:10.8": "dvinnaṃ cittānaṃ samodhānaṃ hotīti?",
"kv16.4:10.9": "Na hevaṃ vattabbe … pe ….",
"kv16.4:11.1": "Anāgataṃ “anāgatan”ti manasikaronto, paccuppannaṃ “paccuppannan”ti manasi karotīti?",
"kv16.4:11.2": "Na hevaṃ vattabbe … pe …",
"kv16.4:11.4": "anāgataṃ “anāgatan”ti manasikaronto, paccuppannaṃ “paccuppannan”ti manasi karotīti?",
"kv16.4:11.5": "Āmantā.",
"kv16.4:11.6": "Dvinnaṃ phassānaṃ … pe …",
"kv16.4:11.8": "dvinnaṃ cittānaṃ samodhānaṃ hotīti?",
"kv16.4:11.9": "Na hevaṃ vattabbe … pe ….",
"kv16.4:12.1": "Anāgataṃ “anāgatan”ti manasikaronto, atītaṃ “atītan”ti manasi karoti, paccuppannaṃ “paccuppannan”ti manasi karotīti?",
"kv16.4:12.2": "Na hevaṃ vattabbe … pe …",
"kv16.4:12.4": "anāgataṃ “anāgatan”ti manasikaronto, atītaṃ “atītan”ti manasi karoti, paccuppannaṃ “paccuppannan”ti manasi karotīti?",
"kv16.4:12.5": "Āmantā.",
"kv16.4:12.6": "Tiṇṇaṃ phassānaṃ … pe …",
"kv16.4:12.8": "tiṇṇaṃ cittānaṃ samodhānaṃ hotīti?",
"kv16.4:12.9": "Na hevaṃ vattabbe … pe ….",
"kv16.4:13.1": "Paccuppannaṃ “paccuppannan”ti manasikaronto, atītaṃ “atītan”ti manasi karotīti?",
"kv16.4:13.2": "Na hevaṃ vattabbe … pe …",
"kv16.4:13.4": "paccuppannaṃ “paccuppannan”ti manasikaronto, atītaṃ “atītan”ti manasi karotīti?",
"kv16.4:13.5": "Āmantā.",
"kv16.4:13.6": "Dvinnaṃ phassānaṃ … pe …",
"kv16.4:13.8": "dvinnaṃ cittānaṃ samodhānaṃ hotīti?",
"kv16.4:13.9": "Na hevaṃ vattabbe … pe ….",
"kv16.4:14.1": "Paccuppannaṃ “paccuppannan”ti manasikaronto, anāgataṃ “anāgatan”ti manasi karotīti?",
"kv16.4:14.2": "Na hevaṃ vattabbe … pe …",
"kv16.4:14.4": "paccuppannaṃ “paccuppannan”ti manasikaronto, anāgataṃ “anāgatan”ti manasi karotīti?",
"kv16.4:14.5": "Āmantā.",
"kv16.4:14.6": "Dvinnaṃ phassānaṃ … pe …",
"kv16.4:14.8": "dvinnaṃ cittānaṃ samodhānaṃ hotīti?",
"kv16.4:14.9": "Na hevaṃ vattabbe … pe ….",
"kv16.4:15.1": "Paccuppannaṃ “paccuppannan”ti manasikaronto, atītaṃ “atītan”ti manasi karoti, anāgataṃ “anāgatan”ti manasi karotīti?",
"kv16.4:15.2": "Na hevaṃ vattabbe … pe …",
"kv16.4:15.4": "paccuppannaṃ “paccuppannan”ti manasikaronto, atītaṃ “atītan”ti manasi karoti, anāgataṃ “anāgatan”ti manasi karotīti?",
"kv16.4:15.5": "Āmantā.",
"kv16.4:15.6": "Tiṇṇaṃ phassānaṃ … pe …",
"kv16.4:15.8": "tiṇṇaṃ cittānaṃ samodhānaṃ hotīti?",
"kv16.4:15.9": "Na hevaṃ vattabbe … pe ….",
"kv16.4:16.1": "☒ Na vattabbaṃ—",
"kv16.4:16.2": "“adhigayha manasi karotī”ti?",
"kv16.4:16.3": "Āmantā.",
"kv16.4:16.4": "Nanu vuttaṃ bhagavatā—",
"kv16.4:17.1": "“Sabbe saṅkhārā aniccāti,",
"kv16.4:17.2": "yadā paññāya passati;",
"kv16.4:17.3": "Atha nibbindati dukkhe,",
"kv16.4:17.4": "esa maggo visuddhiyā.",
"kv16.4:18.1": "Sabbe saṅkhārā dukkhāti,",
"kv16.4:18.2": "yadā paññāya passati;",
"kv16.4:18.3": "Atha nibbindati dukkhe,",
"kv16.4:18.4": "esa maggo visuddhiyā.",
"kv16.4:19.1": "Sabbe dhammā anattāti,",
"kv16.4:19.2": "yadā paññāya passati;",
"kv16.4:19.3": "Atha nibbindati dukkhe,",
"kv16.4:19.4": "esa maggo visuddhiyā”ti.",
"kv16.4:20.1": "Attheva suttantoti?",
"kv16.4:20.2": "Āmantā.",
"kv16.4:20.3": "Tena hi adhigayha manasi karotīti.",
"kv16.4:21.1": "Adhigayhamanasikārakathā niṭṭhitā."
}