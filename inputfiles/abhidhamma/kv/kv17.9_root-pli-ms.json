{
"kv17.9:1.1": "Kathāvatthu",
"kv17.9:2.1": "Mahāpaṇṇāsaka",
"kv17.9:3.1": "Sattarasamavagga",
"kv17.9:4.1": "Navattabbaṃsaṃghassadinnaṃmahapphalantikathā",
"kv17.9:5.1": "Na vattabbaṃ—",
"kv17.9:5.2": "“saṃghassa dinnaṃ mahapphalan”ti?",
"kv17.9:5.3": "Āmantā.",
"kv17.9:5.4": "Nanu saṃgho āhuneyyo pāhuneyyo dakkhiṇeyyo añjalikaraṇīyo anuttaraṃ puññakkhettaṃ lokassāti?",
"kv17.9:5.5": "Āmantā.",
"kv17.9:5.6": "Hañci saṃgho āhuneyyo … pe …",
"kv17.9:5.8": "anuttaraṃ puññakkhettaṃ lokassa, tena vata re vattabbe—",
"kv17.9:5.9": "“saṃghassa dinnaṃ mahapphalan”ti.",
"kv17.9:6.1": "Na vattabbaṃ—",
"kv17.9:6.2": "“saṃghassa dinnaṃ mahapphalan”ti?",
"kv17.9:6.3": "Āmantā.",
"kv17.9:6.4": "Nanu cattāro purisayugā aṭṭha purisapuggalā dakkhiṇeyyā vuttā bhagavatāti?",
"kv17.9:6.5": "Āmantā.",
"kv17.9:6.6": "Hañci cattāro purisayugā aṭṭha purisapuggalā dakkhiṇeyyā vuttā bhagavatā, tena vata re vattabbe—",
"kv17.9:6.7": "“saṃghassa dinnaṃ mahapphalan”ti.",
"kv17.9:7.1": "Na vattabbaṃ—",
"kv17.9:7.2": "“saṃghassa dinnaṃ mahapphalan”ti?",
"kv17.9:7.3": "Āmantā.",
"kv17.9:7.4": "Nanu vuttaṃ bhagavatā—",
"kv17.9:7.5": "“saṃghe, gotami, dehi, saṃghe te dinne ahañceva pūjito bhavissāmi saṃgho cā”ti.",
"kv17.9:7.6": "Attheva suttantoti?",
"kv17.9:7.7": "Āmantā.",
"kv17.9:7.8": "Tena hi saṃghassa dinnaṃ mahapphalanti.",
"kv17.9:8.1": "Na vattabbaṃ—",
"kv17.9:8.2": "“saṃghassa dinnaṃ mahapphalan”ti?",
"kv17.9:8.3": "Āmantā.",
"kv17.9:8.4": "Nanu sakko devānamindo bhagavantaṃ etadavoca—",
"kv17.9:9.1": "“Yajamānānaṃ manussānaṃ,",
"kv17.9:9.2": "puññapekkhāna pāṇinaṃ;",
"kv17.9:9.3": "Karotaṃ opadhikaṃ puññaṃ,",
"kv17.9:9.4": "kattha dinnaṃ mahapphalan”ti.",
"kv17.9:10.1": "“Cattāro ca paṭipannā,",
"kv17.9:10.2": "cattāro ca phale ṭhitā;",
"kv17.9:10.3": "Esa saṃgho ujubhūto,",
"kv17.9:10.4": "paññāsīlasamāhito.",
"kv17.9:11.1": "Yajamānānaṃ manussānaṃ,",
"kv17.9:11.2": "puññapekkhāna pāṇinaṃ;",
"kv17.9:11.3": "Karotaṃ opadhikaṃ puññaṃ,",
"kv17.9:11.4": "saṃghe dinnaṃ mahapphalanti.",
"kv17.9:12.1": "Eso hi saṃgho vipulo mahaggato,",
"kv17.9:12.2": "Esappameyyo udadhīva sāgaro;",
"kv17.9:12.3": "Ete hi seṭṭhā naravīrasāvakā,",
"kv17.9:12.4": "Pabhaṅkarā dhammamudīrayanti.",
"kv17.9:13.1": "Tesaṃ sudinnaṃ suhutaṃ suyiṭṭhaṃ,",
"kv17.9:13.2": "Ye saṃghamuddissa dadanti dānaṃ;",
"kv17.9:13.3": "Sā dakkhiṇā saṃghagatā patiṭṭhitā,",
"kv17.9:13.4": "Mahapphalā lokavidūna vaṇṇitā.",
"kv17.9:14.1": "Etādisaṃ yaññamanussarantā,",
"kv17.9:14.2": "Ye vedajātā vicaranti loke;",
"kv17.9:14.3": "Vineyya maccheramalaṃ samūlaṃ,",
"kv17.9:14.4": "Aninditā saggamupenti ṭhānan”ti.",
"kv17.9:15.1": "Attheva suttantoti?",
"kv17.9:15.2": "Āmantā.",
"kv17.9:15.3": "Tena hi saṃghassa dinnaṃ mahapphalanti.",
"kv17.9:16.1": "Na vattabbaṃ saṃghassa dinnaṃ mahapphalantikathā niṭṭhitā."
}