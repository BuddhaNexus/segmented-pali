{
"kv11.7:1.1": "Kathāvatthu",
"kv11.7:2.1": "Mahāpaṇṇāsaka",
"kv11.7:3.1": "Ekādasamavagga",
"kv11.7:4.1": "Dhammaṭṭhitatākathā",
"kv11.7:5.1": "Dhammaṭṭhitatā parinipphannāti?",
"kv11.7:5.2": "Āmantā.",
"kv11.7:5.3": "Tāya ṭhitatā parinipphannāti?",
"kv11.7:5.4": "Na hevaṃ vattabbe … pe …",
"kv11.7:5.6": "tāya ṭhitatā parinipphannāti?",
"kv11.7:5.7": "Āmantā.",
"kv11.7:5.8": "Tāya tāyeva natthi dukkhassantakiriyā, natthi vaṭṭupacchedo, natthi anupādāparinibbānanti?",
"kv11.7:5.9": "Na hevaṃ vattabbe … pe ….",
"kv11.7:6.1": "Rūpassa ṭhitatā parinipphannāti?",
"kv11.7:6.2": "Āmantā.",
"kv11.7:6.3": "Tāya ṭhitatā parinipphannāti?",
"kv11.7:6.4": "Na hevaṃ vattabbe … pe …",
"kv11.7:6.6": "tāya ṭhitatā parinipphannāti?",
"kv11.7:6.7": "Āmantā.",
"kv11.7:6.8": "Tāya tāyeva natthi dukkhassantakiriyā, natthi vaṭṭupacchedo, natthi anupādāparinibbānanti?",
"kv11.7:6.9": "Na hevaṃ vattabbe … pe ….",
"kv11.7:7.1": "Vedanāya ṭhitatā … pe …",
"kv11.7:7.3": "saññāya ṭhitatā … pe …",
"kv11.7:7.5": "saṅkhārānaṃ ṭhitatā … pe …",
"kv11.7:7.7": "viññāṇassa ṭhitatā parinipphannāti?",
"kv11.7:7.8": "Āmantā.",
"kv11.7:7.9": "Tāya ṭhitatā parinipphannāti?",
"kv11.7:7.10": "Na hevaṃ vattabbe … pe …",
"kv11.7:7.12": "tāya ṭhitatā parinipphannāti?",
"kv11.7:7.13": "Āmantā.",
"kv11.7:7.14": "Tāya tāyeva natthi dukkhassantakiriyā, natthi vaṭṭupacchedo, natthi anupādāparinibbānanti?",
"kv11.7:7.15": "Na hevaṃ vattabbe … pe ….",
"kv11.7:8.1": "Dhammaṭṭhitatākathā niṭṭhitā."
}