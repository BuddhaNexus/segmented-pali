{
"kv15.4:1.1": "Kathāvatthu",
"kv15.4:2.1": "Mahāpaṇṇāsaka",
"kv15.4:3.1": "Pannarasamavagga",
"kv15.4:4.1": "Khaṇalayamuhuttakathā",
"kv15.4:5.1": "Khaṇo parinipphanno, layo parinipphanno, muhuttaṃ parinipphannanti?",
"kv15.4:5.2": "Āmantā.",
"kv15.4:5.3": "Rūpanti?",
"kv15.4:5.4": "Na hevaṃ vattabbe … pe …",
"kv15.4:5.6": "vedanā …",
"kv15.4:5.7": "saññā …",
"kv15.4:5.8": "saṅkhārā …",
"kv15.4:5.9": "viññāṇanti?",
"kv15.4:5.10": "Na hevaṃ vattabbe … pe ….",
"kv15.4:6.1": "Na vattabbaṃ—",
"kv15.4:6.2": "“muhuttaṃ parinipphannan”ti?",
"kv15.4:6.3": "Āmantā.",
"kv15.4:6.4": "Nanu vuttaṃ bhagavatā—",
"kv15.4:6.5": "“tīṇimāni, bhikkhave, kathāvatthūni.",
"kv15.4:6.6": "Katamāni tīṇi?",
"kv15.4:6.7": "Atītaṃ vā, bhikkhave, addhānaṃ ārabbha kathaṃ katheyya—",
"kv15.4:6.8": "‘evaṃ ahosi atītamaddhānan’ti;",
"kv15.4:6.9": "anāgataṃ vā, bhikkhave, addhānaṃ ārabbha kathaṃ katheyya—",
"kv15.4:6.10": "‘evaṃ bhavissati anāgatamaddhānan’ti;",
"kv15.4:6.11": "etarahi vā, bhikkhave, paccuppannaṃ addhānaṃ ārabbha kathaṃ katheyya—",
"kv15.4:6.12": "‘evaṃ hoti etarahi paccuppannan’ti.",
"kv15.4:6.13": "Imāni kho, bhikkhave, tīṇi kathāvatthūnī”ti.",
"kv15.4:6.14": "Attheva suttantoti?",
"kv15.4:6.15": "Āmantā.",
"kv15.4:6.16": "Tena hi muhuttaṃ parinipphannanti.",
"kv15.4:7.1": "Khaṇalayamuhuttakathā niṭṭhitā."
}