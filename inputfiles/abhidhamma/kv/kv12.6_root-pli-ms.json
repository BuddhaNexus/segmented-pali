{
"kv12.6:1.1": "Kathāvatthu",
"kv12.6:2.1": "Mahāpaṇṇāsaka",
"kv12.6:3.1": "Dvādasamavagga",
"kv12.6:4.1": "Kolaṃkolakathā",
"kv12.6:5.1": "☒ Na vattabbaṃ—",
"kv12.6:5.2": "“kolaṃkolo puggalo kolaṃkolatāniyato”ti?",
"kv12.6:5.3": "Āmantā.",
"kv12.6:5.4": "Nanu so kolaṃkoloti?",
"kv12.6:5.5": "Āmantā.",
"kv12.6:5.6": "Hañci so kolaṃkolo, tena vata re vattabbe—",
"kv12.6:5.7": "“kolaṃkolo puggalo kolaṃkolatāniyato”ti.",
"kv12.6:6.1": "Kolaṃkolakathā niṭṭhitā."
}