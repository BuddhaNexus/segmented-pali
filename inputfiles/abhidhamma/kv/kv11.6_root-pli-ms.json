{
"kv11.6:1.1": "Kathāvatthu",
"kv11.6:2.1": "Mahāpaṇṇāsaka",
"kv11.6:3.1": "Ekādasamavagga",
"kv11.6:4.1": "Samādhikathā",
"kv11.6:5.1": "Cittasantati samādhīti?",
"kv11.6:5.2": "Āmantā.",
"kv11.6:5.3": "Atītā cittasantati samādhīti?",
"kv11.6:5.4": "Na hevaṃ vattabbe … pe …",
"kv11.6:5.6": "cittasantati samādhīti?",
"kv11.6:5.7": "Āmantā.",
"kv11.6:5.8": "Anāgatā cittasantati samādhīti?",
"kv11.6:5.9": "Na hevaṃ vattabbe … pe …",
"kv11.6:5.11": "cittasantati samādhīti?",
"kv11.6:5.12": "Āmantā.",
"kv11.6:5.13": "Nanu atītaṃ niruddhaṃ anāgataṃ ajātanti?",
"kv11.6:5.14": "Āmantā.",
"kv11.6:5.15": "Hañci atītaṃ niruddhaṃ anāgataṃ ajātaṃ, no ca vata re vattabbe—",
"kv11.6:5.16": "“cittasantati samādhī”ti.",
"kv11.6:6.1": "☒ Ekacittakkhaṇiko samādhīti?",
"kv11.6:6.2": "Āmantā.",
"kv11.6:6.3": "Cakkhuviññāṇasamaṅgī samāpannoti?",
"kv11.6:6.4": "Na hevaṃ vattabbe … pe …",
"kv11.6:6.6": "sotaviññāṇasamaṅgī … pe …",
"kv11.6:6.8": "ghānaviññāṇasamaṅgī …",
"kv11.6:6.9": "jivhāviññāṇasamaṅgī …",
"kv11.6:6.10": "kāyaviññāṇasamaṅgī … pe …",
"kv11.6:6.12": "akusalacittasamaṅgī … pe …",
"kv11.6:6.14": "rāgasahagatacittasamaṅgī … pe …",
"kv11.6:6.16": "dosasahagatacittasamaṅgī … pe …",
"kv11.6:6.18": "mohasahagatacittasamaṅgī … pe …",
"kv11.6:6.20": "anottappasahagatacittasamaṅgī samāpannoti?",
"kv11.6:6.21": "Na hevaṃ vattabbe … pe ….",
"kv11.6:7.1": "Cittasantati samādhīti?",
"kv11.6:7.2": "Āmantā.",
"kv11.6:7.3": "Akusalacittasantati samādhīti?",
"kv11.6:7.4": "Na hevaṃ vattabbe … pe …",
"kv11.6:7.6": "rāgasahagatā … pe …",
"kv11.6:7.8": "dosasahagatā … pe …",
"kv11.6:7.10": "mohasahagatā … pe …",
"kv11.6:7.12": "anottappasahagatā cittasantati samādhīti?",
"kv11.6:7.13": "Na hevaṃ vattabbe … pe ….",
"kv11.6:8.1": "☒ Na vattabbaṃ—",
"kv11.6:8.2": "“cittasantati samādhī”ti?",
"kv11.6:8.3": "Āmantā.",
"kv11.6:8.4": "Nanu vuttaṃ bhagavatā—",
"kv11.6:8.5": "“ahaṃ kho, āvuso nigaṇṭhā, pahomi aniñjamāno kāyena, abhāsamāno vācaṃ, satta rattindivāni ekantasukhaṃ paṭisaṃvedī viharitun”ti.",
"kv11.6:8.6": "Attheva suttantoti?",
"kv11.6:8.7": "Āmantā.",
"kv11.6:8.8": "Tena hi cittasantati samādhīti.",
"kv11.6:9.1": "Samādhikathā niṭṭhitā."
}