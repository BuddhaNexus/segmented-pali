{
"kv17.5:1.1": "Kathāvatthu",
"kv17.5:2.1": "Mahāpaṇṇāsaka",
"kv17.5:3.1": "Sattarasamavagga",
"kv17.5:4.1": "Ṭhapetvāariyamaggantikathā",
"kv17.5:5.1": "Ṭhapetvā ariyamaggaṃ avasesā saṅkhārā dukkhāti?",
"kv17.5:5.2": "Āmantā.",
"kv17.5:5.3": "Dukkhasamudayopi dukkhoti?",
"kv17.5:5.4": "Na hevaṃ vattabbe … pe …",
"kv17.5:5.6": "dukkhasamudayopi dukkhoti?",
"kv17.5:5.7": "Āmantā.",
"kv17.5:5.8": "Tīṇeva ariyasaccānīti?",
"kv17.5:5.9": "Na hevaṃ vattabbe … pe …",
"kv17.5:5.11": "tīṇeva ariyasaccānīti?",
"kv17.5:5.12": "Āmantā.",
"kv17.5:5.13": "Nanu cattāri ariyasaccāni vuttāni bhagavatā—",
"kv17.5:5.14": "dukkhaṃ, dukkhasamudayo, dukkhanirodho, dukkhanirodhagāminī paṭipadāti?",
"kv17.5:5.15": "Āmantā.",
"kv17.5:5.16": "Hañci cattāri ariyasaccāni vuttāni bhagavatā—",
"kv17.5:5.17": "dukkhaṃ, dukkhasamudayo, dukkhanirodho, dukkhanirodhagāminī paṭipadā;",
"kv17.5:5.18": "no ca vata re vattabbe—",
"kv17.5:5.19": "“tīṇeva ariyasaccānī”ti.",
"kv17.5:6.1": "Dukkhasamudayopi dukkhoti?",
"kv17.5:6.2": "Āmantā.",
"kv17.5:6.3": "Kenaṭṭhenāti?",
"kv17.5:6.4": "Aniccaṭṭhena.",
"kv17.5:6.5": "Ariyamaggo aniccoti?",
"kv17.5:6.6": "Āmantā.",
"kv17.5:6.7": "Ariyamaggo dukkhoti?",
"kv17.5:6.8": "Na hevaṃ vattabbe … pe ….",
"kv17.5:7.1": "Ariyamaggo anicco, so ca na dukkhoti?",
"kv17.5:7.2": "Āmantā.",
"kv17.5:7.3": "Dukkhasamudayo anicco, so ca na dukkhoti?",
"kv17.5:7.4": "Na hevaṃ vattabbe … pe …",
"kv17.5:7.6": "dukkhasamudayo anicco, so ca dukkhoti?",
"kv17.5:7.7": "Āmantā.",
"kv17.5:7.8": "Ariyamaggo anicco, so ca dukkhoti?",
"kv17.5:7.9": "Na hevaṃ vattabbe … pe ….",
"kv17.5:8.1": "☒ Na vattabbaṃ—",
"kv17.5:8.2": "“ṭhapetvā ariyamaggaṃ avasesā saṅkhārā dukkhā”ti?",
"kv17.5:8.3": "Āmantā.",
"kv17.5:8.4": "Nanu sā dukkhanirodhagāminī paṭipadāti?",
"kv17.5:8.5": "Āmantā.",
"kv17.5:8.6": "Hañci sā dukkhanirodhagāminī paṭipadā, tena vata re vattabbe—",
"kv17.5:8.7": "“ṭhapetvā ariyamaggaṃ avasesā saṅkhārā dukkhā”ti.",
"kv17.5:9.1": "Ṭhapetvā ariyamaggantikathā niṭṭhitā."
}