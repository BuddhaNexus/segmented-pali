{
"kv16.2:1.1": "Kathāvatthu",
"kv16.2:2.1": "Niggahapaṇṇāsaka",
"kv16.2:3.1": "Soḷasamavagga",
"kv16.2:4.1": "Paggahakathā",
"kv16.2:5.1": "Paro parassa cittaṃ paggaṇhātīti?",
"kv16.2:5.2": "Āmantā.",
"kv16.2:5.3": "Paro parassa cittaṃ “mā rajji”, “mā dussi”, “mā muyhi”, “mā kilissī”ti paggaṇhātīti?",
"kv16.2:5.4": "Na hevaṃ vattabbe … pe …",
"kv16.2:5.6": "paro parassa cittaṃ paggaṇhātīti?",
"kv16.2:5.7": "Āmantā.",
"kv16.2:5.8": "Paro parassa alobhaṃ kusalamūlaṃ janeti …",
"kv16.2:5.9": "adosaṃ kusalamūlaṃ janeti …",
"kv16.2:5.10": "amohaṃ kusalamūlaṃ janeti …",
"kv16.2:5.11": "saddhaṃ janeti …",
"kv16.2:5.12": "vīriyaṃ janeti …",
"kv16.2:5.13": "satiṃ janeti …",
"kv16.2:5.14": "samādhiṃ janeti …",
"kv16.2:5.15": "paññaṃ janetīti?",
"kv16.2:5.16": "Na hevaṃ vattabbe … pe …",
"kv16.2:5.18": "paro parassa cittaṃ paggaṇhātīti?",
"kv16.2:5.19": "Āmantā.",
"kv16.2:5.20": "Paro parassa uppanno phasso “mā nirujjhī”ti paggaṇhātīti?",
"kv16.2:5.21": "Na hevaṃ vattabbe … pe …",
"kv16.2:5.23": "paro parassa uppannā vedanā … pe …",
"kv16.2:5.25": "uppannā paññā “mā nirujjhī”ti paggaṇhātīti?",
"kv16.2:5.26": "Na hevaṃ vattabbe … pe ….",
"kv16.2:6.1": "Paro parassa cittaṃ paggaṇhātīti?",
"kv16.2:6.2": "Āmantā.",
"kv16.2:6.3": "Paro parassa atthāya rāgaṃ pajahati …",
"kv16.2:6.4": "dosaṃ pajahati …",
"kv16.2:6.5": "mohaṃ pajahati … pe …",
"kv16.2:6.7": "anottappaṃ pajahatīti?",
"kv16.2:6.8": "Na hevaṃ vattabbe … pe ….",
"kv16.2:7.1": "Paro parassa cittaṃ paggaṇhātīti?",
"kv16.2:7.2": "Āmantā.",
"kv16.2:7.3": "Paro parassa atthāya maggaṃ bhāveti …",
"kv16.2:7.4": "satipaṭṭhānaṃ bhāveti … pe …",
"kv16.2:7.6": "bojjhaṅgaṃ bhāvetīti?",
"kv16.2:7.7": "Na hevaṃ vattabbe … pe ….",
"kv16.2:8.1": "Paro parassa cittaṃ paggaṇhātīti?",
"kv16.2:8.2": "Āmantā.",
"kv16.2:8.3": "Paro parassa atthāya dukkhaṃ parijānāti … pe …",
"kv16.2:8.5": "maggaṃ bhāvetīti?",
"kv16.2:8.6": "Na hevaṃ vattabbe … pe ….",
"kv16.2:9.1": "Paro parassa cittaṃ paggaṇhātīti?",
"kv16.2:9.2": "Āmantā.",
"kv16.2:9.3": "Añño aññassa kārako, paraṃkataṃ sukhaṃ dukkhaṃ añño karoti añño paṭisaṃvedetīti?",
"kv16.2:9.4": "Na hevaṃ vattabbe … pe ….",
"kv16.2:10.1": "Paro parassa cittaṃ paggaṇhātīti?",
"kv16.2:10.2": "Āmantā.",
"kv16.2:10.3": "Nanu vuttaṃ bhagavatā—",
"kv16.2:11.1": "“Attanāva kataṃ pāpaṃ, … pe …",
"kv16.2:11.4": "Nāñño aññaṃ visodhaye”ti.",
"kv16.2:12.1": "Attheva suttantoti?",
"kv16.2:12.2": "Āmantā.",
"kv16.2:12.3": "Tena hi na vattabbaṃ—",
"kv16.2:12.4": "“paro parassa cittaṃ paggaṇhātī”ti.",
"kv16.2:13.1": "☒ Na vattabbaṃ—",
"kv16.2:13.2": "“paro parassa cittaṃ paggaṇhātī”ti?",
"kv16.2:13.3": "Āmantā.",
"kv16.2:13.4": "Nanu atthi balappattā, atthi vasībhūtāti?",
"kv16.2:13.5": "Āmantā.",
"kv16.2:13.6": "Hañci atthi balappattā, atthi vasībhūtā, tena vata re vattabbe—",
"kv16.2:13.7": "“paro parassa cittaṃ paggaṇhātī”ti.",
"kv16.2:14.1": "Paggahakathā niṭṭhitā."
}