{
"dt2.8:1.1": "Dhātukathā",
"dt2.8:2.1": "2 Niddesa",
"dt2.8:3.1": "2.8. Aṭṭhamanaya Vippayuttenasampayuttapadaniddesa",
"dt2.8:4.1": "Rūpakkhandhena ye dhammā vippayuttā, te dhammā katihi khandhehi katihāyatanehi katihi dhātūhi sampayuttāti?",
"dt2.8:4.2": "Natthi.",
"dt2.8:5.1": "Vedanākkhandhena ye dhammā …",
"dt2.8:5.2": "saññākkhandhena ye dhammā …",
"dt2.8:5.3": "saṅkhārakkhandhena ye dhammā …",
"dt2.8:5.4": "viññāṇakkhandhena ye dhammā … pe …",
"dt2.8:5.6": "saraṇehi dhammehi ye dhammā …",
"dt2.8:5.7": "araṇehi dhammehi ye dhammā vippayuttā, te dhammā katihi khandhehi katihāyatanehi katihi dhātūhi sampayuttāti?",
"dt2.8:5.8": "Natthi.",
"dt2.8:6.1": "Dhammāyatanaṃ dhammadhātu,",
"dt2.8:6.2": "Atha jīvitaṃ nāmarūpaṃ;",
"dt2.8:6.3": "Saḷāyatanaṃ jātijarāmataṃ,",
"dt2.8:6.4": "Dve ca tike na labbhare.",
"dt2.8:7.1": "Paṭhamantare satta ca,",
"dt2.8:7.2": "Gocchake dasa aparante;",
"dt2.8:7.3": "Cuddasa cha ca matthake,",
"dt2.8:7.4": "Iccete sattacattālīsa dhammā;",
"dt2.8:7.5": "Samucchede na labbhanti,",
"dt2.8:7.6": "Moghapucchakena cāti. #47/47",
"dt2.8:8.1": "Vippayuttena sampayuttapadaniddeso aṭṭhamo."
}