{
"ya4.1.2:1.1": "4 Dhātuyamaka",
"ya4.1.2:2.1": "4.1 Paṇṇattivāra",
"ya4.1.2:3.1": "4.1.2. Paṇṇattivāraniddesa",
"ya4.1.2:4.1": "4.1.2.1. Padasodhanavāra",
"ya4.1.2:5.1": "4.1.2.11. Anuloma",
"ya4.1.2:6.1": "⇒ Cakkhu cakkhudhātūti?",
"ya4.1.2:7.1": "Dibbacakkhu paññācakkhu cakkhu, na cakkhudhātu.",
"ya4.1.2:7.2": "Cakkhudhātu cakkhu ceva cakkhudhātu ca.",
"ya4.1.2:8.1": "⇐ Cakkhudhātu cakkhūti?",
"ya4.1.2:8.2": "Āmantā.",
"ya4.1.2:9.1": "⇒ Sotaṃ sotadhātūti?",
"ya4.1.2:10.1": "Dibbasotaṃ taṇhāsotaṃ sotaṃ, na sotadhātu.",
"ya4.1.2:10.2": "Sotadhātu sotañceva sotadhātu ca.",
"ya4.1.2:11.1": "⇐ Sotadhātu sotanti?",
"ya4.1.2:11.2": "Āmantā.",
"ya4.1.2:12.1": "⇒ Ghānaṃ ghānadhātūti?",
"ya4.1.2:12.2": "Āmantā.",
"ya4.1.2:13.1": "⇐ Ghānadhātu ghānanti?",
"ya4.1.2:13.2": "Āmantā.",
"ya4.1.2:14.1": "(Jivhāpi ghānadhātusadisā.)",
"ya4.1.2:15.1": "⇒ Kāyo kāyadhātūti?",
"ya4.1.2:16.1": "Kāyadhātuṃ ṭhapetvā avaseso kāyo, na kāyadhātu.",
"ya4.1.2:16.2": "Kāyadhātu kāyo ceva kāyadhātu ca.",
"ya4.1.2:17.1": "⇐ Kāyadhātu kāyoti?",
"ya4.1.2:17.2": "Āmantā.",
"ya4.1.2:18.1": "⇒ Rūpaṃ rūpadhātūti?",
"ya4.1.2:19.1": "Rūpadhātuṃ ṭhapetvā avasesaṃ rūpaṃ, na rūpadhātu.",
"ya4.1.2:19.2": "Rūpadhātu rūpañceva rūpadhātu ca.",
"ya4.1.2:20.1": "⇐ Rūpadhātu rūpanti?",
"ya4.1.2:20.2": "Āmantā.",
"ya4.1.2:21.1": "(Saddo ghānasadiso.)",
"ya4.1.2:22.1": "⇒ Gandho gandhadhātūti?",
"ya4.1.2:23.1": "Sīlagandho samādhigandho paññāgandho gandho, na gandhadhātu.",
"ya4.1.2:23.2": "Gandhadhātu gandho ceva gandhadhātu ca.",
"ya4.1.2:24.1": "⇐ Gandhadhātu gandhoti?",
"ya4.1.2:24.2": "Āmantā.",
"ya4.1.2:25.1": "⇒ Raso rasadhātūti?",
"ya4.1.2:26.1": "Attharaso dhammaraso vimuttiraso raso, na rasadhātu.",
"ya4.1.2:26.2": "Rasadhātu raso ceva rasadhātu ca.",
"ya4.1.2:27.1": "⇐ Rasadhātu rasoti?",
"ya4.1.2:27.2": "Āmantā.",
"ya4.1.2:28.1": "(Phoṭṭhabbo ghānasadiso.)",
"ya4.1.2:29.1": "⇒ Cakkhuviññāṇaṃ cakkhuviññāṇadhātūti?",
"ya4.1.2:29.2": "Āmantā.",
"ya4.1.2:30.1": "⇐ Cakkhuviññāṇadhātu cakkhuviññāṇanti?",
"ya4.1.2:30.2": "Āmantā.",
