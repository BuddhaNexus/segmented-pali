{
  "pp2.8:0.1": "Puggalapaññatti",
  "pp2.8:0.2": "Niddesa",
  "pp2.8:0.3": "8. Aṭṭhakapuggalapaññatti",
  "pp2.8:1.1": "Tattha katame cattāro maggasamaṅgino, cattāro phalasamaṅgino puggalā?",
  "pp2.8:1.2": "Sotāpanno, sotāpattiphalasacchikiriyāya paṭipanno;",
  "pp2.8:1.3": "sakadāgāmī, sakadāgāmiphalasacchikiriyāya paṭipanno;",
  "pp2.8:1.4": "anāgāmī, anāgāmiphalasacchikiriyāya paṭipanno;",
  "pp2.8:1.5": "arahā, arahattaphalasacchikiriyāya paṭipanno;",
  "pp2.8:1.6": "ime cattāro maggasamaṅgino, ime cattāro phalasamaṅgino puggalā.",
  "pp2.8:2.1": "Aṭṭhakaniddeso."
}