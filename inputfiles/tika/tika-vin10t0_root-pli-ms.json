{
  "tika-vin10t0:0": " Namo tassa bhagavato arahato sammāsambuddhassa",
  "tika-vin10t0:1": "Vinayavinicchayo",
  "tika-vin10t0:2": "Ganthārambhakathā",
  "tika-vin10t0:3": "1.",
  "tika-vin10t0:4": " Vanditvā sirasā seṭṭhaṁ, buddhamappaṭipuggalaṁ;",
  "tika-vin10t0:5": "Bhavābhāvakaraṁ dhammaṁ, gaṇañceva niraṅgaṇaṁ.",
  "tika-vin10t0:6": "2.",
  "tika-vin10t0:7": " Bhikkhūnaṁ bhikkhunīnañca, hitatthāya samāhito;",
  "tika-vin10t0:8": "Pavakkhāmi samāsena, vinayassavinicchayaṁ.",
  "tika-vin10t0:9": "3.",
  "tika-vin10t0:10": " Anākulamasaṁkiṇṇaṁ, madhuratthapadakkamaṁ;",
  "tika-vin10t0:11": "Paṭubhāvakaraṁ etaṁ, paramaṁ vinayakkame.",
  "tika-vin10t0:12": "4.",
  "tika-vin10t0:13": " Apāraṁ otarantānaṁ, sāraṁ vinayasāgaraṁ;",
  "tika-vin10t0:14": "Bhikkhūnaṁ bhikkhunīnañca, nāvābhūtaṁ manoramaṁ.",
  "tika-vin10t0:15": "5.",
  "tika-vin10t0:16": " Tasmā vinayanūpāyaṁ, vinayassavinicchayaṁ;",
  "tika-vin10t0:17": "Avikkhittena cittena, vadato me nibodhatha."
}