{
  "tika-abh06t72:0.1": "Saccasaṅkhepo",
  "tika-abh06t72:0.2": "Ganthārambhakathā",
  "tika-abh06t72:1": "1.",
  "tika-abh06t72:2": " Namassitvā tilokaggaṃ, ñeyyasāgarapāraguṃ;",
  "tika-abh06t72:3": "Bhavābhāvakaraṃ dhammaṃ, gaṇañca guṇasāgaraṃ.",
  "tika-abh06t72:4": "2.",
  "tika-abh06t72:5": " Nissāya pubbācariyamataṃ atthāvirodhinaṃ;",
  "tika-abh06t72:6": "Vakkhāmi saccasaṅkhepaṃ, hitaṃ kārakayoginaṃ."
}