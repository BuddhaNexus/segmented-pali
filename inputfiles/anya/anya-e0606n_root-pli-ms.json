{
  "anya-e0606n:0": " Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e0606n:1": "Kamalāñjali",
  "anya-e0606n:2": "1.",
  "anya-e0606n:3": " Kamalāsana kamalāpati pamathādhipa vajirā,",
  "anya-e0606n:4": "Yudhadānava manujoraga bhujagāsana patinaṃ;",
  "anya-e0606n:5": "Makuṭāhita manidīdhiti bhamarāvali bhajitaṃ,",
  "anya-e0606n:6": "Paṇamā’mahaṃ anaghaṃ muni caraṇāmala kamalaṃ.",
  "anya-e0606n:7": "2.",
  "anya-e0606n:8": " Kamalālaya sadi’saṅkita kalamaṅgala ruciraṃ,",
  "anya-e0606n:9": "Jutirañjitaṃ abhinijjita subhakañcana nalinaṃ;",
  "anya-e0606n:10": "Janitā cira samaye nija pitu bhūpati namitaṃ,",
  "anya-e0606n:11": "Paṇamā’mahaṃ anaghaṃ muni caraṇāmala kamalaṃ.",
  "anya-e0606n:12": "3.",
  "anya-e0606n:13": " Saraṇāgata rajanīpati dinasekhara namitaṃ,",
  "anya-e0606n:14": "Bhajitākhila janapāvanaṃ abhikaṅkhita sukhadaṃ;",
  "anya-e0606n:15": "Vasudhātala sayamuggata sarasīruha mahitaṃ,",
  "anya-e0606n:16": "Paṇamā’mahaṃ anaghaṃ muni caraṇāmala kamalaṃ.",
  "anya-e0606n:17": "4.",
  "anya-e0606n:18": " Diradāsana tarupantika ghaṭitāsanaṃ abhayaṃ,",
  "anya-e0606n:19": "Vijayāsana samadhiṭṭhita caturaṅgika viriyaṃ;",
  "anya-e0606n:20": "Dharamānaka suriyehani vijitantaka dhajiniṃ,",
  "anya-e0606n:21": "Paṇamā’mahaṃ abhipātita namuciddhaja vibhavaṃ.",
  "anya-e0606n:22": "5.",
  "anya-e0606n:23": " Sadayodita piyabhārati vijitantaka samaraṃ,",
  "anya-e0606n:24": "Dasapārami balakampita sadharādhara dharaniṃ;",
  "anya-e0606n:25": "Girimekhala varavārana sirasānata caraṇaṃ,",
  "anya-e0606n:26": "Paṇamā’mahaṃ abhinandita sanarāmara bhuvanaṃ.",
  "anya-e0606n:27": "6.",
  "anya-e0606n:28": " Nikhilāsava vigame’nativimalīkata hadayaṃ,",
  "anya-e0606n:29": "Tadanantara viditākhila matigocara visayaṃ;",
  "anya-e0606n:30": "Visayīkata bhuvanattayaṃ atilokiya caritaṃ,",
  "anya-e0606n:31": "Paṇamā’mahaṃ aparājitaṃ arahaṃ muniṃ asamaṃ.",
  "anya-e0606n:32": "7.",
  "anya-e0606n:33": " Mudubhārati madhupāsita nalinopama vadanaṃ,",
  "anya-e0606n:34": "Rucirāyata nalinīdala nibha locana yugalaṃ;",
  "anya-e0606n:35": "Udayodita ravimaṇḍala jalitāmala niṭilaṃ,",
  "anya-e0606n:36": "Paṇamā’maha akutobhayaṃ anaghaṃ muni pamukhaṃ.",
  "anya-e0606n:37": "8.",
  "anya-e0606n:38": " Asitambuda rucikuñcita mudu kuntala lalitaṃ,",
  "anya-e0606n:39": "Bhuvanodara vitatāmita jutisañcaya jalitaṃ;",
  "anya-e0606n:40": "Madamodita diradopama gativibbhama ruciraṃ,",
  "anya-e0606n:41": "Paṇamā’mahaṃ amatandada munipuṅgavaṃ asamaṃ.",
  "anya-e0606n:42": "9.",
  "anya-e0606n:43": " Karuṇārasa paribhāvita savaṇāmata vacanaṃ,",
  "anya-e0606n:44": "Virudāvali sataghosita yasapūrita bhuvanaṃ;",
  "anya-e0606n:45": "Sumanohara varalakkhaṇa sirisañcaya sadanaṃ,",
  "anya-e0606n:46": "Paṇamā’mahaṃ uditāmala sasimaṇḍala vadanaṃ.",
  "anya-e0606n:47": "10.",
  "anya-e0606n:48": " Vinayāraha janamānasa kumudākara sasinaṃ,",
  "anya-e0606n:49": "Tasināpaga parisosana satadīdhiti tulitaṃ;",
  "anya-e0606n:50": "Tamanāsava munisevitaṃ apalokita sukhadaṃ,",
  "anya-e0606n:51": "Paṇamā’mahaṃ aniketanaṃ akhilāgati vigataṃ.",
  "anya-e0606n:52": "11.",
  "anya-e0606n:53": " Sahitākhila bhayabheravaṃ abhayāgata saranaṃ,",
  "anya-e0606n:54": "Ajarāmara sukhadāyakaṃ anirākata karuṇaṃ;",
  "anya-e0606n:55": "Tamupāsaka janasevita supatiṭṭhita caraṇaṃ,",
  "anya-e0606n:56": "Paṇamā’mahaṃ ahitāpahaṃ anaghuttama caraṇaṃ.",
  "anya-e0606n:57": "12.",
  "anya-e0606n:58": " Karuṇāmata rasapurita vīmalākhila hadayaṃ,",
  "anya-e0606n:59": "Vihitāmita janatāhitaṃ anukampita bhuvanaṃ;",
  "anya-e0606n:60": "Bhuvane sutaṃ avanīpati sata sevita caraṇaṃ,",
  "anya-e0606n:61": "Paṇamā’mahaṃ anaghaṃ muniṃ aghanāsana caturaṃ.",
  "anya-e0606n:62": "13.",
  "anya-e0606n:63": " Aratīrati paripīlita yatimānasa damanaṃ,",
  "anya-e0606n:64": "Nijasāsana vinivārita puthutitthiya samaṇaṃ;",
  "anya-e0606n:65": "Paravādika janatākata paribhāsita khamanaṃ,",
  "anya-e0606n:66": "Paṇamā’mahaṃ atidevata vara gotama samaṇaṃ.",
  "anya-e0606n:67": "14.",
  "anya-e0606n:68": " Saraṇāgata bhayanāsana vajirālaya paṇibhaṃ,",
  "anya-e0606n:69": "Bhavasāgara patitāmita janatārana nirataṃ;",
  "anya-e0606n:70": "Sirasāvahaṃ amalañjali puṭapaṅkaja makulaṃ,",
  "anya-e0606n:71": "Paṇamā’mahaṃ akhilalāya vigataṃ muniṃ atulaṃ.",
  "anya-e0606n:72": "15.",
  "anya-e0606n:73": " Vimalīkata janamānasa vigatāsava bhagavaṃ,",
  "anya-e0606n:74": "Bhavapāraga vibhavāmata sukhadāyaka satataṃ;",
  "anya-e0606n:75": "Paramādara garugārava vinataṃ jina payataṃ,",
  "anya-e0606n:76": "Padapaṅkaja rajasā mama samalaṅkuru sirasaṃ.",
  "anya-e0606n:77": "16.",
  "anya-e0606n:78": " Pavanāhata dumapallavaṃ iva nārata capalaṃ,",
  "anya-e0606n:79": "Bhavalālasa malinīkataṃ ajitindriya nivahaṃ;",
  "anya-e0606n:80": "Cira sañcita duritāhataṃ anivārita timisaṃ,",
  "anya-e0606n:81": "Vimalīkuru karuṇābhara sutaraṃ mama hadayaṃ.",
  "anya-e0606n:82": "17.",
  "anya-e0606n:83": " Adaye dayaṃ anaye nayaṃ api yo guṇaṃ aguṇe,",
  "anya-e0606n:84": "Ahite hitaṃ akaro kvacida api kenaci nakataṃ;",
  "anya-e0606n:85": "Sadaye jina sunaye guṇasadane tayi nitaraṃ,",
  "anya-e0606n:86": "Suhite hitacarite’nagha ramate mama hadayaṃ.",
  "anya-e0606n:87": "18.",
  "anya-e0606n:88": " Bhavasaṅkaṭa patitenapi bhavatā cira caritaṃ,",
  "anya-e0606n:89": "Visame sama caraṇaṃ khalu dasapārami bharaṇaṃ;",
  "anya-e0606n:90": "Sarato’hani sarato nisi supinenapi satataṃ,",
  "anya-e0606n:91": "Ramate jina sumate tvayi sadayaṃ mama hadayaṃ.",
  "anya-e0606n:92": "19.",
  "anya-e0606n:93": " Atiduddadaṃ adadī bhavaṃ atidukkaraṃ akarī;",
  "anya-e0606n:94": "Atidukkhamaṃ akhamī vata karuṇānidhi’rasamo,",
  "anya-e0606n:95": "Iti te guṇaṃ anaghaṃ muni sarato mama hadayaṃ,",
  "anya-e0606n:96": "Ramate’hani ramate nisi ramate tvayi satataṃ.",
  "anya-e0606n:97": "20.",
  "anya-e0606n:98": " Atiduccaraṃ acarī bhavaṃ atiduddamaṃ adamī,",
  "anya-e0606n:99": "Atiduddayaṃ adayī vata sadayāpara hadayo;",
  "anya-e0606n:100": "Iti te guṇaṃ anaghaṃ muni sarato mama hadayaṃ,",
  "anya-e0606n:101": "Ramate’hani ramate nisi ramate tvayi satataṃ.",
  "anya-e0606n:102": "21.",
  "anya-e0606n:103": " Atiduggamaṃ agamī bhavaṃ atidujjayaṃ ajayī,",
  "anya-e0606n:104": "Atidussahaṃ asahī vata samupekkhita manaso;",
  "anya-e0606n:105": "Iti te guṇaṃ anaghaṃ muni sarato mama hadayaṃ,",
  "anya-e0606n:106": "Ramate’hani ramate nisi ramate tvayi satataṃ.",
  "anya-e0606n:107": "22.",
  "anya-e0606n:108": " Atidāruna palayānala sadisānala jalite,",
  "anya-e0606n:109": "Niraye vinipatito ciraṃ aghatāpita manaso;",
  "anya-e0606n:110": "Na sariṃ sakida api te pita bhajituṃ padanalinaṃ,",
  "anya-e0606n:111": "Khama gotama narasārathi tamidaṃ mama khalitaṃ.",
  "anya-e0606n:112": "23.",
  "anya-e0606n:113": " Tiriyaggata-gatiyaṃ ciraṃ anavaṭṭhita carito,",
  "anya-e0606n:114": "Atiniṭṭhura vadhatajjita bhayakampita hadayo;",
  "anya-e0606n:115": "Na sariṃ sakidapi te pita bhajituṃ padanalinaṃ,",
  "anya-e0606n:116": "Khama gotama purisuttama tamidaṃ mama khalitaṃ.",
  "anya-e0606n:117": "24.",
  "anya-e0606n:118": " Paridevana nirato ciraṃ atha pettiya visaye,",
  "anya-e0606n:119": "Sujighacchita supipāsita parisosita jaṭhare;",
  "anya-e0606n:120": "Na sariṃ sakidapi te pita bhajituṃ padanalinaṃ,",
  "anya-e0606n:121": "Khama gotama dipaduttama tamidaṃ mama khalitaṃ.",
  "anya-e0606n:122": "25.",
  "anya-e0606n:123": " Vivaso bhusaṃ aghadūsita manasāsura visaye,",
  "anya-e0606n:124": "Janito ghanatimire ciraṃ atidukkhita hadayo;",
  "anya-e0606n:125": "Na sariṃ sakidapi te pita bhajituṃ padanalinaṃ,",
  "anya-e0606n:126": "Khama gotama vasisattama tamidaṃ mama khalitaṃ.",
  "anya-e0606n:127": "26.",
  "anya-e0606n:128": " Manasā cira vihitaṃ saraṃ atikibbisa caritaṃ,",
  "anya-e0606n:129": "Samathenatha suvirājiya taṃ asaññitaṃ upago;",
  "anya-e0606n:130": "Na sariṃ sakida api te pita bhajituṃ pada nalinaṃ,",
  "anya-e0606n:131": "Khama gotama vasipuṅgava tamidaṃ mama khalitaṃ.",
  "anya-e0606n:132": "27.",
  "anya-e0606n:133": " Vijigucchiya duritaṃ nija vapusā kataṃ amitaṃ,",
  "anya-e0606n:134": "Tanu vajjitaṃ upago bhavaṃ iha bhāvita samatho;",
  "anya-e0606n:135": "Na sariṃ sakidapi te pita bhajituṃ padanalinaṃ,",
  "anya-e0606n:136": "Khama gotama yatikuñjara tamidaṃ mama khalitaṃ.",
  "anya-e0606n:137": "28.",
  "anya-e0606n:138": " Ratanattaya rahite bhusa bahulīkata durite,",
  "anya-e0606n:139": "Janito paravisaye budhajana nindiya carite;",
  "anya-e0606n:140": "Na sariṃ sakidapi te pita bhajituṃ padanalinaṃ,",
  "anya-e0606n:141": "Khama gotama karuṇānidhi tamidaṃ mama khalitaṃ.",
  "anya-e0606n:142": "29.",
  "anya-e0606n:143": " Janito yadi manujesupi vikalindriya nivaho,",
  "anya-e0606n:144": "Tanunā karacaraṇādihi vikate’niha dukhito;",
  "anya-e0606n:145": "Na sariṃ sakidapi te pita bhajituṃ padanalinaṃ,",
  "anya-e0606n:146": "Khama gotama matisāgara tamidaṃ mama khalitaṃ.",
  "anya-e0606n:147": "30.",
  "anya-e0606n:148": " Vidhināhita matibhāvana rahito tamapihito,",
  "anya-e0606n:149": "Visadesupi kusalādisu tathadassana vimukho;",
  "anya-e0606n:150": "Na sariṃ sakidapi te pita bhajituṃ padanalinaṃ,",
  "anya-e0606n:151": "Khama gotama narakesari tamidaṃ mama khalitaṃ.",
  "anya-e0606n:152": "31.",
  "anya-e0606n:153": " Sucirenapi bhuvi dullabhaṃ asamaṃ khaṇaṃ alabhaṃ,",
  "anya-e0606n:154": "Sanarāmara janatāhita sukhadaṃ muni jananaṃ;",
  "anya-e0606n:155": "Na sariṃ sakidapi te pita bhajituṃ padanalinaṃ,",
  "anya-e0606n:156": "Khama gotama vadataṃvara tamidaṃ mama khalitaṃ.",
  "anya-e0606n:157": "32.",
  "anya-e0606n:158": " Nisitāyudha vadhasajjita khaḷaniddaya hadayo,",
  "anya-e0606n:159": "Parahiṃsana ruci bhiṃsana yamasodara sadiso;",
  "anya-e0606n:160": "Na sariṃ sakidapi te pita bhajituṃ padanalinaṃ,",
  "anya-e0606n:161": "Khama gotama munisattama tamidaṃ mama khalitaṃ.",
  "anya-e0606n:162": "33.",
  "anya-e0606n:163": " Parasantaka haraṇe katamati bañcana bahulo,",
  "anya-e0606n:164": "Gharasandhika paripanthika sahasākati nirato;",
  "anya-e0606n:165": "Na sariṃ sakidapi te pita bhajituṃ padanalinaṃ,",
  "anya-e0606n:166": "Khama gotama munipuṅgava tamidaṃ mama khalitaṃ.",
  "anya-e0606n:167": "34.",
  "anya-e0606n:168": " Navayobbana madagabbita parimucchita hadayo,",
  "anya-e0606n:169": "Sucisajjana vijigucchiya paradārika nirato;",
  "anya-e0606n:170": "Na sariṃ sakidapi te pita bhajituṃ padanalinaṃ,",
  "anya-e0606n:171": "Khama gotama karuṇābhara tamidaṃ mama khalitaṃ.",
  "anya-e0606n:172": "35.",
  "anya-e0606n:173": " Madirāsavarata nāgarajana santata bhajito,",
  "anya-e0606n:174": "Garugārava hiridūrita tiriyaggata carito;",
  "anya-e0606n:175": "Na sariṃ sakidapi te pita bhajituṃ padanalinaṃ,",
  "anya-e0606n:176": "Khama gotama vihatāsava tamidaṃ mama khalitaṃ.",
  "anya-e0606n:177": "36.",
  "anya-e0606n:178": " Sapitāmaha papitāmaha nicitaṃ dhanaṃ amitaṃ,",
  "anya-e0606n:179": "Pitusañcitamapi nāsiya kitavo hatavibhavo;",
  "anya-e0606n:180": "Na sariṃ sakidapi te pita bhajituṃ padanalinaṃ,",
  "anya-e0606n:181": "Khama gotama vijitantaka tamidaṃ mama khalitaṃ.",
  "anya-e0606n:182": "37.",
  "anya-e0606n:183": " Vitathālika vacano parapiyasuññata karaṇo,",
  "anya-e0606n:184": "Pharusaṃ bhaṇaṃ atinipphala bahubhāsana nipuṇo;",
  "anya-e0606n:185": "Na sariṃ sakidapi te pita bhajituṃ padanalinaṃ,",
  "anya-e0606n:186": "Khama gotama bhuvanesuta tamidaṃ mama khalitaṃ.",
  "anya-e0606n:187": "38.",
  "anya-e0606n:188": " Parasampadaṃ abhijhāyana nirantara dukhito,",
  "anya-e0606n:189": "Nabhirajjhana paramo kvaci phaladassana rahito;",
  "anya-e0606n:190": "Na sariṃ sakidapi te pita bhajituṃ padanalinaṃ,",
  "anya-e0606n:191": "Khama gotama guṇasāgara tamidaṃ mama khalitaṃ.",
  "anya-e0606n:192": "39.",
  "anya-e0606n:193": " Bhavato bhavaṃ aparāparaṃ ayatāciraṃ iti me,",
  "anya-e0606n:194": "Vapusā atha vacasāpi ca manasā kataṃ amitaṃ;",
  "anya-e0606n:195": "Khama gotama duritāpaha duritaṃ bahuvihitaṃ,",
  "anya-e0606n:196": "Dada me sivapadaṃ accutaṃ amataṃ bhavavigataṃ.",
  "anya-e0606n:197": "40.",
  "anya-e0606n:198": " Timirāvuta kuṇapākula vijigucchiya pavane,",
  "anya-e0606n:199": "Janikāsuci jaṭhare bahu kimisantati sadane;",
  "anya-e0606n:200": "Asayiṃ bhavagahaṇe caraṃ ahaṃ appaṭisaraṇe,",
  "anya-e0606n:201": "Saranaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:202": "41.",
  "anya-e0606n:203": " Bahi nikkhamaṃ asakiṃ bhagatiriyaṃ patha patito,",
  "anya-e0606n:204": "Agadaṅkara kata sallaka satakhaṇḍita karaṇo;",
  "anya-e0606n:205": "Amariṃ bhavagahaṇe caraṃ ahaṃ appaṭisaraṇe,",
  "anya-e0606n:206": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi tarane.",
  "anya-e0606n:207": "42.",
  "anya-e0606n:208": " Patito bahi ratipillaka tanurāmaya mathito,",
  "anya-e0606n:209": "Vadituṃ kimu viditumpi ca na sahaṃ matirahito;",
  "anya-e0606n:210": "Amariṃ bhavagahaṇe caraṃ ahaṃ apphṭisaraṇe,",
  "anya-e0606n:211": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:212": "43.",
  "anya-e0606n:213": " Janito yadi sukhito janadayito piyajanako,",
  "anya-e0606n:214": "Puthuko bahuvidha-kīlananirato gadagahito;",
  "anya-e0606n:215": "Amariṃ bhavagahaṇe caraṃ ahaṃ appaṭisaraṇe,",
  "anya-e0606n:216": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:217": "44.",
  "anya-e0606n:218": " Tarunopi hi gharabandhana gathitomita vibhavo,",
  "anya-e0606n:219": "Sahasā gadagahito piyabhariyāsuta viyuto;",
  "anya-e0606n:220": "Amariṃ bhavagahaṇe caraṃ ahaṃ appaṭisaraṇe,",
  "anya-e0606n:221": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:222": "45.",
  "anya-e0606n:223": " Ajaraṃ tanuṃ abhimaññiya navayobbana vasiko,",
  "anya-e0606n:224": "Jarasā parimathito paraṃ ananuṭṭhita kusalo;",
  "anya-e0606n:225": "Amariṃ bhavagahaṇe caraṃ ahaṃ appaṭisaraṇe,",
  "anya-e0606n:226": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:227": "46.",
  "anya-e0606n:228": " Nirujo dhuvaṃ arujaṃ tanuṃ abhimaññiya samado,",
  "anya-e0606n:229": "Kusalāsaya vimukho bhusaṃ avaso gadanihato;",
  "anya-e0606n:230": "Amariṃ bhavagahaṇe caraṃ ahaṃ appaṭisaraṇe,",
  "anya-e0606n:231": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:232": "47.",
  "anya-e0606n:233": " Pavicintiya sakajīvitaṃ amaraṃ dhuvaṃ anighaṃ,",
  "anya-e0606n:234": "Iti jīvitamadagabbita matirujjhita kusalo;",
  "anya-e0606n:235": "Amariṃ bhavagahaṇe caraṃ ahaṃ appaṭisaraṇe,",
  "anya-e0606n:236": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:237": "48.",
  "anya-e0606n:238": " Parahiṃsana dhanamosana paradārika nirato,",
  "anya-e0606n:239": "Dharanīpati gahito bahu vadhabandhana nihato;",
  "anya-e0606n:240": "Amariṃ bhavagahaṇe caraṃ ahaṃ appaṭisaraṇe,",
  "anya-e0606n:241": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:242": "49.",
  "anya-e0606n:243": " Narakodaka patito giritarumatthaka galito,",
  "anya-e0606n:244": "Migavāḷaka gahito visadhara jātihi ḍasito;",
  "anya-e0606n:245": "Amariṃ bhavagahaṇe caraṃ ahaṃ appaṭisaraṇe,",
  "anya-e0606n:246": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:247": "50.",
  "anya-e0606n:248": " Abhicāraka vidhikopita nisicāraka gahito,",
  "anya-e0606n:249": "Savisodana sahasādana pabhutīhi ca khalito;",
  "anya-e0606n:250": "Amariṃ bhavagahaṇe caraṃ ahaṃ appaṭisaraṇe,",
  "anya-e0606n:251": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:252": "51.",
  "anya-e0606n:253": " Sayamevaca sajanopari kupito mativiyuto,",
  "anya-e0606n:254": "Savisādana galakantana pabhutāmita khalito;",
  "anya-e0606n:255": "Amariṃ bhavagahaṇe caraṃ ahaṃ appaṭisaraṇe,",
  "anya-e0606n:256": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:257": "52.",
  "anya-e0606n:258": " Pitupūjana niratāsaya sukhito pitudayito,",
  "anya-e0606n:259": "Maraṇenaca pituno bhusaṃ anusocana nirato;",
  "anya-e0606n:260": "Parudiṃ ciraṃ atidussaha kasire bhavagahaṇe,",
  "anya-e0606n:261": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:262": "53.",
  "anya-e0606n:263": " Samupaṭṭhiya janikaṃ nijaṃ abhivādana paramo,",
  "anya-e0606n:264": "Maraṇenaca janikāyanusaritā guṇamahimaṃ;",
  "anya-e0606n:265": "Parudiṃ ciraṃ atidussaha kasire bhavagahaṇe,",
  "anya-e0606n:266": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:267": "54.",
  "anya-e0606n:268": " Gurudevata patimānana paramo piyasuvaco,",
  "anya-e0606n:269": "Samupāsitacaraṇo gurumaraṇenatidukhito;",
  "anya-e0606n:270": "Parudiṃ ciraṃ atidussaha kasire bhavagahaṇe,",
  "anya-e0606n:271": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:272": "55.",
  "anya-e0606n:273": " Gharamedhitaṃ upagomitavibhavo ratibahulo,",
  "anya-e0606n:274": "Maraṇe piyabhariyāsutaduhitū’natikaruṇaṃ;",
  "anya-e0606n:275": "Parudiṃ ciraṃ atidussaha kasire bhavagahaṇe,",
  "anya-e0606n:276": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:277": "56.",
  "anya-e0606n:278": " Piyapubbaja sahajānuja bhagiṇidvaya maraṇe,",
  "anya-e0606n:279": "Nija bandhava-sakha-sissaka maraṇe pyatikaruṇaṃ;",
  "anya-e0606n:280": "Parudiṃ ciraṃ atidussaha kasire bhavagahaṇe,",
  "anya-e0606n:281": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:282": "57.",
  "anya-e0606n:283": " Jagatīpati gahaṇā ripujanatakkara haraṇā,",
  "anya-e0606n:284": "Saritodaka vahaṇā puthujalitānala dahaṇā;",
  "anya-e0606n:285": "Parudiṃ hatavibhavo ciramiha duggatigahaṇe,",
  "anya-e0606n:286": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:287": "58.",
  "anya-e0606n:288": " Atiduggama visamākula bhavasaṅkaṭa patite,",
  "anya-e0606n:289": "Byasanaṃ ciramiti dussahaṃ anubhūyapi vimitaṃ;",
  "anya-e0606n:290": "Na jahe sukhalavavañcitahadayo bhavatasinaṃ,",
  "anya-e0606n:291": "Tamapākuru karuṇānidhi tasinaṃ mama kasiṇaṃ.",
  "anya-e0606n:292": "59.",
  "anya-e0606n:293": " Jananāvadhi maraṇaṃ viya maraṇāvadhi jananaṃ,",
  "anya-e0606n:294": "Ubhayenapi bhayamevahi bhavato mama niyataṃ;",
  "anya-e0606n:295": "Sivamevaca jananāvadhi maraṇāvadhi rahitaṃ,",
  "anya-e0606n:296": "Dada me sivaṃ amatandada taṃ anāsava bhagavaṃ.",
  "anya-e0606n:297": "60.",
  "anya-e0606n:298": " Ciradikkhitamapi me manaṃ anivārita tasinaṃ,",
  "anya-e0606n:299": "Bhavato bhava ratipīlitaṃ ahaho kalighaṭitaṃ!",
  "anya-e0606n:300": "Tamato pita bhayato mamamava me bhava saraṇaṃ,",
  "anya-e0606n:301": "Bhagavaṃ paṭisaraṇaṃ mama bhavanīradhi taraṇaṃ."
}