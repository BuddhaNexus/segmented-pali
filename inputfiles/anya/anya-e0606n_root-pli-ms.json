{
  "anya-e0606n:6": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e0606n:7": "Kamalāñjali",
  "anya-e0606n:8": "1.",
  "anya-e0606n:9": "Kamalāsana kamalāpati pamathādhipa vajirā,",
  "anya-e0606n:10": "Yudhadānava manujoraga bhujagāsana patinaṃ;",
  "anya-e0606n:11": "Makuṭāhita manidīdhiti bhamarāvali bhajitaṃ,",
  "anya-e0606n:12": "Paṇamā’mahaṃ anaghaṃ muni caraṇāmala kamalaṃ.",
  "anya-e0606n:13": "2.",
  "anya-e0606n:14": "Kamalālaya sadi’saṅkita kalamaṅgala ruciraṃ,",
  "anya-e0606n:15": "Jutirañjitaṃ abhinijjita subhakañcana nalinaṃ;",
  "anya-e0606n:16": "Janitā cira samaye nija pitu bhūpati namitaṃ,",
  "anya-e0606n:17": "Paṇamā’mahaṃ anaghaṃ muni caraṇāmala kamalaṃ.",
  "anya-e0606n:18": "3.",
  "anya-e0606n:19": "Saraṇāgata rajanīpati dinasekhara namitaṃ,",
  "anya-e0606n:20": "Bhajitākhila janapāvanaṃ abhikaṅkhita sukhadaṃ;",
  "anya-e0606n:21": "Vasudhātala sayamuggata sarasīruha mahitaṃ,",
  "anya-e0606n:22": "Paṇamā’mahaṃ anaghaṃ muni caraṇāmala kamalaṃ.",
  "anya-e0606n:23": "4.",
  "anya-e0606n:24": "Diradāsana tarupantika ghaṭitāsanaṃ abhayaṃ,",
  "anya-e0606n:25": "Vijayāsana samadhiṭṭhita caturaṅgika viriyaṃ;",
  "anya-e0606n:26": "Dharamānaka suriyehani vijitantaka dhajiniṃ,",
  "anya-e0606n:27": "Paṇamā’mahaṃ abhipātita namuciddhaja vibhavaṃ.",
  "anya-e0606n:28": "5.",
  "anya-e0606n:29": "Sadayodita piyabhārati vijitantaka samaraṃ,",
  "anya-e0606n:30": "Dasapārami balakampita sadharādhara dharaniṃ;",
  "anya-e0606n:31": "Girimekhala varavārana sirasānata caraṇaṃ,",
  "anya-e0606n:32": "Paṇamā’mahaṃ abhinandita sanarāmara bhuvanaṃ.",
  "anya-e0606n:33": "6.",
  "anya-e0606n:34": "Nikhilāsava vigame’nativimalīkata hadayaṃ,",
  "anya-e0606n:35": "Tadanantara viditākhila matigocara visayaṃ;",
  "anya-e0606n:36": "Visayīkata bhuvanattayaṃ atilokiya caritaṃ,",
  "anya-e0606n:37": "Paṇamā’mahaṃ aparājitaṃ arahaṃ muniṃ asamaṃ.",
  "anya-e0606n:38": "7.",
  "anya-e0606n:39": "Mudubhārati madhupāsita nalinopama vadanaṃ,",
  "anya-e0606n:40": "Rucirāyata nalinīdala nibha locana yugalaṃ;",
  "anya-e0606n:41": "Udayodita ravimaṇḍala jalitāmala niṭilaṃ,",
  "anya-e0606n:42": "Paṇamā’maha akutobhayaṃ anaghaṃ muni pamukhaṃ.",
  "anya-e0606n:43": "8.",
  "anya-e0606n:44": "Asitambuda rucikuñcita mudu kuntala lalitaṃ,",
  "anya-e0606n:45": "Bhuvanodara vitatāmita jutisañcaya jalitaṃ;",
  "anya-e0606n:46": "Madamodita diradopama gativibbhama ruciraṃ,",
  "anya-e0606n:47": "Paṇamā’mahaṃ amatandada munipuṅgavaṃ asamaṃ.",
  "anya-e0606n:48": "9.",
  "anya-e0606n:49": "Karuṇārasa paribhāvita savaṇāmata vacanaṃ,",
  "anya-e0606n:50": "Virudāvali sataghosita yasapūrita bhuvanaṃ;",
  "anya-e0606n:51": "Sumanohara varalakkhaṇa sirisañcaya sadanaṃ,",
  "anya-e0606n:52": "Paṇamā’mahaṃ uditāmala sasimaṇḍala vadanaṃ.",
  "anya-e0606n:53": "10.",
  "anya-e0606n:54": "Vinayāraha janamānasa kumudākara sasinaṃ,",
  "anya-e0606n:55": "Tasināpaga parisosana satadīdhiti tulitaṃ;",
  "anya-e0606n:56": "Tamanāsava munisevitaṃ apalokita sukhadaṃ,",
  "anya-e0606n:57": "Paṇamā’mahaṃ aniketanaṃ akhilāgati vigataṃ.",
  "anya-e0606n:58": "11.",
  "anya-e0606n:59": "Sahitākhila bhayabheravaṃ abhayāgata saranaṃ,",
  "anya-e0606n:60": "Ajarāmara sukhadāyakaṃ anirākata karuṇaṃ;",
  "anya-e0606n:61": "Tamupāsaka janasevita supatiṭṭhita caraṇaṃ,",
  "anya-e0606n:62": "Paṇamā’mahaṃ ahitāpahaṃ anaghuttama caraṇaṃ.",
  "anya-e0606n:63": "12.",
  "anya-e0606n:64": "Karuṇāmata rasapurita vīmalākhila hadayaṃ,",
  "anya-e0606n:65": "Vihitāmita janatāhitaṃ anukampita bhuvanaṃ;",
  "anya-e0606n:66": "Bhuvane sutaṃ avanīpati sata sevita caraṇaṃ,",
  "anya-e0606n:67": "Paṇamā’mahaṃ anaghaṃ muniṃ aghanāsana caturaṃ.",
  "anya-e0606n:68": "13.",
  "anya-e0606n:69": "Aratīrati paripīlita yatimānasa damanaṃ,",
  "anya-e0606n:70": "Nijasāsana vinivārita puthutitthiya samaṇaṃ;",
  "anya-e0606n:71": "Paravādika janatākata paribhāsita khamanaṃ,",
  "anya-e0606n:72": "Paṇamā’mahaṃ atidevata vara gotama samaṇaṃ.",
  "anya-e0606n:73": "14.",
  "anya-e0606n:74": "Saraṇāgata bhayanāsana vajirālaya paṇibhaṃ,",
  "anya-e0606n:75": "Bhavasāgara patitāmita janatārana nirataṃ;",
  "anya-e0606n:76": "Sirasāvahaṃ amalañjali puṭapaṅkaja makulaṃ,",
  "anya-e0606n:77": "Paṇamā’mahaṃ akhilalāya vigataṃ muniṃ atulaṃ.",
  "anya-e0606n:78": "15.",
  "anya-e0606n:79": "Vimalīkata janamānasa vigatāsava bhagavaṃ,",
  "anya-e0606n:80": "Bhavapāraga vibhavāmata sukhadāyaka satataṃ;",
  "anya-e0606n:81": "Paramādara garugārava vinataṃ jina payataṃ,",
  "anya-e0606n:82": "Padapaṅkaja rajasā mama samalaṅkuru sirasaṃ.",
  "anya-e0606n:83": "16.",
  "anya-e0606n:84": "Pavanāhata dumapallavaṃ iva nārata capalaṃ,",
  "anya-e0606n:85": "Bhavalālasa malinīkataṃ ajitindriya nivahaṃ;",
  "anya-e0606n:86": "Cira sañcita duritāhataṃ anivārita timisaṃ,",
  "anya-e0606n:87": "Vimalīkuru karuṇābhara sutaraṃ mama hadayaṃ.",
  "anya-e0606n:88": "17.",
  "anya-e0606n:89": "Adaye dayaṃ anaye nayaṃ api yo guṇaṃ aguṇe,",
  "anya-e0606n:90": "Ahite hitaṃ akaro kvacida api kenaci nakataṃ;",
  "anya-e0606n:91": "Sadaye jina sunaye guṇasadane tayi nitaraṃ,",
  "anya-e0606n:92": "Suhite hitacarite’nagha ramate mama hadayaṃ.",
  "anya-e0606n:93": "18.",
  "anya-e0606n:94": "Bhavasaṅkaṭa patitenapi bhavatā cira caritaṃ,",
  "anya-e0606n:95": "Visame sama caraṇaṃ khalu dasapārami bharaṇaṃ;",
  "anya-e0606n:96": "Sarato’hani sarato nisi supinenapi satataṃ,",
  "anya-e0606n:97": "Ramate jina sumate tvayi sadayaṃ mama hadayaṃ.",
  "anya-e0606n:98": "19.",
  "anya-e0606n:99": "Atiduddadaṃ adadī bhavaṃ atidukkaraṃ akarī;",
  "anya-e0606n:100": "Atidukkhamaṃ akhamī vata karuṇānidhi’rasamo,",
  "anya-e0606n:101": "Iti te guṇaṃ anaghaṃ muni sarato mama hadayaṃ,",
  "anya-e0606n:102": "Ramate’hani ramate nisi ramate tvayi satataṃ.",
  "anya-e0606n:103": "20.",
  "anya-e0606n:104": "Atiduccaraṃ acarī bhavaṃ atiduddamaṃ adamī,",
  "anya-e0606n:105": "Atiduddayaṃ adayī vata sadayāpara hadayo;",
  "anya-e0606n:106": "Iti te guṇaṃ anaghaṃ muni sarato mama hadayaṃ,",
  "anya-e0606n:107": "Ramate’hani ramate nisi ramate tvayi satataṃ.",
  "anya-e0606n:108": "21.",
  "anya-e0606n:109": "Atiduggamaṃ agamī bhavaṃ atidujjayaṃ ajayī,",
  "anya-e0606n:110": "Atidussahaṃ asahī vata samupekkhita manaso;",
  "anya-e0606n:111": "Iti te guṇaṃ anaghaṃ muni sarato mama hadayaṃ,",
  "anya-e0606n:112": "Ramate’hani ramate nisi ramate tvayi satataṃ.",
  "anya-e0606n:113": "22.",
  "anya-e0606n:114": "Atidāruna palayānala sadisānala jalite,",
  "anya-e0606n:115": "Niraye vinipatito ciraṃ aghatāpita manaso;",
  "anya-e0606n:116": "Na sariṃ sakida api te pita bhajituṃ padanalinaṃ,",
  "anya-e0606n:117": "Khama gotama narasārathi tamidaṃ mama khalitaṃ.",
  "anya-e0606n:118": "23.",
  "anya-e0606n:119": "Tiriyaggata-gatiyaṃ ciraṃ anavaṭṭhita carito,",
  "anya-e0606n:120": "Atiniṭṭhura vadhatajjita bhayakampita hadayo;",
  "anya-e0606n:121": "Na sariṃ sakidapi te pita bhajituṃ padanalinaṃ,",
  "anya-e0606n:122": "Khama gotama purisuttama tamidaṃ mama khalitaṃ.",
  "anya-e0606n:123": "24.",
  "anya-e0606n:124": "Paridevana nirato ciraṃ atha pettiya visaye,",
  "anya-e0606n:125": "Sujighacchita supipāsita parisosita jaṭhare;",
  "anya-e0606n:126": "Na sariṃ sakidapi te pita bhajituṃ padanalinaṃ,",
  "anya-e0606n:127": "Khama gotama dipaduttama tamidaṃ mama khalitaṃ.",
  "anya-e0606n:128": "25.",
  "anya-e0606n:129": "Vivaso bhusaṃ aghadūsita manasāsura visaye,",
  "anya-e0606n:130": "Janito ghanatimire ciraṃ atidukkhita hadayo;",
  "anya-e0606n:131": "Na sariṃ sakidapi te pita bhajituṃ padanalinaṃ,",
  "anya-e0606n:132": "Khama gotama vasisattama tamidaṃ mama khalitaṃ.",
  "anya-e0606n:133": "26.",
  "anya-e0606n:134": "Manasā cira vihitaṃ saraṃ atikibbisa caritaṃ,",
  "anya-e0606n:135": "Samathenatha suvirājiya taṃ asaññitaṃ upago;",
  "anya-e0606n:136": "Na sariṃ sakida api te pita bhajituṃ pada nalinaṃ,",
  "anya-e0606n:137": "Khama gotama vasipuṅgava tamidaṃ mama khalitaṃ.",
  "anya-e0606n:138": "27.",
  "anya-e0606n:139": "Vijigucchiya duritaṃ nija vapusā kataṃ amitaṃ,",
  "anya-e0606n:140": "Tanu vajjitaṃ upago bhavaṃ iha bhāvita samatho;",
  "anya-e0606n:141": "Na sariṃ sakidapi te pita bhajituṃ padanalinaṃ,",
  "anya-e0606n:142": "Khama gotama yatikuñjara tamidaṃ mama khalitaṃ.",
  "anya-e0606n:143": "28.",
  "anya-e0606n:144": "Ratanattaya rahite bhusa bahulīkata durite,",
  "anya-e0606n:145": "Janito paravisaye budhajana nindiya carite;",
  "anya-e0606n:146": "Na sariṃ sakidapi te pita bhajituṃ padanalinaṃ,",
  "anya-e0606n:147": "Khama gotama karuṇānidhi tamidaṃ mama khalitaṃ.",
  "anya-e0606n:148": "29.",
  "anya-e0606n:149": "Janito yadi manujesupi vikalindriya nivaho,",
  "anya-e0606n:150": "Tanunā karacaraṇādihi vikate’niha dukhito;",
  "anya-e0606n:151": "Na sariṃ sakidapi te pita bhajituṃ padanalinaṃ,",
  "anya-e0606n:152": "Khama gotama matisāgara tamidaṃ mama khalitaṃ.",
  "anya-e0606n:153": "30.",
  "anya-e0606n:154": "Vidhināhita matibhāvana rahito tamapihito,",
  "anya-e0606n:155": "Visadesupi kusalādisu tathadassana vimukho;",
  "anya-e0606n:156": "Na sariṃ sakidapi te pita bhajituṃ padanalinaṃ,",
  "anya-e0606n:157": "Khama gotama narakesari tamidaṃ mama khalitaṃ.",
  "anya-e0606n:158": "31.",
  "anya-e0606n:159": "Sucirenapi bhuvi dullabhaṃ asamaṃ khaṇaṃ alabhaṃ,",
  "anya-e0606n:160": "Sanarāmara janatāhita sukhadaṃ muni jananaṃ;",
  "anya-e0606n:161": "Na sariṃ sakidapi te pita bhajituṃ padanalinaṃ,",
  "anya-e0606n:162": "Khama gotama vadataṃvara tamidaṃ mama khalitaṃ.",
  "anya-e0606n:163": "32.",
  "anya-e0606n:164": "Nisitāyudha vadhasajjita khaḷaniddaya hadayo,",
  "anya-e0606n:165": "Parahiṃsana ruci bhiṃsana yamasodara sadiso;",
  "anya-e0606n:166": "Na sariṃ sakidapi te pita bhajituṃ padanalinaṃ,",
  "anya-e0606n:167": "Khama gotama munisattama tamidaṃ mama khalitaṃ.",
  "anya-e0606n:168": "33.",
  "anya-e0606n:169": "Parasantaka haraṇe katamati bañcana bahulo,",
  "anya-e0606n:170": "Gharasandhika paripanthika sahasākati nirato;",
  "anya-e0606n:171": "Na sariṃ sakidapi te pita bhajituṃ padanalinaṃ,",
  "anya-e0606n:172": "Khama gotama munipuṅgava tamidaṃ mama khalitaṃ.",
  "anya-e0606n:173": "34.",
  "anya-e0606n:174": "Navayobbana madagabbita parimucchita hadayo,",
  "anya-e0606n:175": "Sucisajjana vijigucchiya paradārika nirato;",
  "anya-e0606n:176": "Na sariṃ sakidapi te pita bhajituṃ padanalinaṃ,",
  "anya-e0606n:177": "Khama gotama karuṇābhara tamidaṃ mama khalitaṃ.",
  "anya-e0606n:178": "35.",
  "anya-e0606n:179": "Madirāsavarata nāgarajana santata bhajito,",
  "anya-e0606n:180": "Garugārava hiridūrita tiriyaggata carito;",
  "anya-e0606n:181": "Na sariṃ sakidapi te pita bhajituṃ padanalinaṃ,",
  "anya-e0606n:182": "Khama gotama vihatāsava tamidaṃ mama khalitaṃ.",
  "anya-e0606n:183": "36.",
  "anya-e0606n:184": "Sapitāmaha papitāmaha nicitaṃ dhanaṃ amitaṃ,",
  "anya-e0606n:185": "Pitusañcitamapi nāsiya kitavo hatavibhavo;",
  "anya-e0606n:186": "Na sariṃ sakidapi te pita bhajituṃ padanalinaṃ,",
  "anya-e0606n:187": "Khama gotama vijitantaka tamidaṃ mama khalitaṃ.",
  "anya-e0606n:188": "37.",
  "anya-e0606n:189": "Vitathālika vacano parapiyasuññata karaṇo,",
  "anya-e0606n:190": "Pharusaṃ bhaṇaṃ atinipphala bahubhāsana nipuṇo;",
  "anya-e0606n:191": "Na sariṃ sakidapi te pita bhajituṃ padanalinaṃ,",
  "anya-e0606n:192": "Khama gotama bhuvanesuta tamidaṃ mama khalitaṃ.",
  "anya-e0606n:193": "38.",
  "anya-e0606n:194": "Parasampadaṃ abhijhāyana nirantara dukhito,",
  "anya-e0606n:195": "Nabhirajjhana paramo kvaci phaladassana rahito;",
  "anya-e0606n:196": "Na sariṃ sakidapi te pita bhajituṃ padanalinaṃ,",
  "anya-e0606n:197": "Khama gotama guṇasāgara tamidaṃ mama khalitaṃ.",
  "anya-e0606n:198": "39.",
  "anya-e0606n:199": "Bhavato bhavaṃ aparāparaṃ ayatāciraṃ iti me,",
  "anya-e0606n:200": "Vapusā atha vacasāpi ca manasā kataṃ amitaṃ;",
  "anya-e0606n:201": "Khama gotama duritāpaha duritaṃ bahuvihitaṃ,",
  "anya-e0606n:202": "Dada me sivapadaṃ accutaṃ amataṃ bhavavigataṃ.",
  "anya-e0606n:203": "40.",
  "anya-e0606n:204": "Timirāvuta kuṇapākula vijigucchiya pavane,",
  "anya-e0606n:205": "Janikāsuci jaṭhare bahu kimisantati sadane;",
  "anya-e0606n:206": "Asayiṃ bhavagahaṇe caraṃ ahaṃ appaṭisaraṇe,",
  "anya-e0606n:207": "Saranaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:208": "41.",
  "anya-e0606n:209": "Bahi nikkhamaṃ asakiṃ bhagatiriyaṃ patha patito,",
  "anya-e0606n:210": "Agadaṅkara kata sallaka satakhaṇḍita karaṇo;",
  "anya-e0606n:211": "Amariṃ bhavagahaṇe caraṃ ahaṃ appaṭisaraṇe,",
  "anya-e0606n:212": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi tarane.",
  "anya-e0606n:213": "42.",
  "anya-e0606n:214": "Patito bahi ratipillaka tanurāmaya mathito,",
  "anya-e0606n:215": "Vadituṃ kimu viditumpi ca na sahaṃ matirahito;",
  "anya-e0606n:216": "Amariṃ bhavagahaṇe caraṃ ahaṃ apphṭisaraṇe,",
  "anya-e0606n:217": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:218": "43.",
  "anya-e0606n:219": "Janito yadi sukhito janadayito piyajanako,",
  "anya-e0606n:220": "Puthuko bahuvidha-kīlananirato gadagahito;",
  "anya-e0606n:221": "Amariṃ bhavagahaṇe caraṃ ahaṃ appaṭisaraṇe,",
  "anya-e0606n:222": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:223": "44.",
  "anya-e0606n:224": "Tarunopi hi gharabandhana gathitomita vibhavo,",
  "anya-e0606n:225": "Sahasā gadagahito piyabhariyāsuta viyuto;",
  "anya-e0606n:226": "Amariṃ bhavagahaṇe caraṃ ahaṃ appaṭisaraṇe,",
  "anya-e0606n:227": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:228": "45.",
  "anya-e0606n:229": "Ajaraṃ tanuṃ abhimaññiya navayobbana vasiko,",
  "anya-e0606n:230": "Jarasā parimathito paraṃ ananuṭṭhita kusalo;",
  "anya-e0606n:231": "Amariṃ bhavagahaṇe caraṃ ahaṃ appaṭisaraṇe,",
  "anya-e0606n:232": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:233": "46.",
  "anya-e0606n:234": "Nirujo dhuvaṃ arujaṃ tanuṃ abhimaññiya samado,",
  "anya-e0606n:235": "Kusalāsaya vimukho bhusaṃ avaso gadanihato;",
  "anya-e0606n:236": "Amariṃ bhavagahaṇe caraṃ ahaṃ appaṭisaraṇe,",
  "anya-e0606n:237": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:238": "47.",
  "anya-e0606n:239": "Pavicintiya sakajīvitaṃ amaraṃ dhuvaṃ anighaṃ,",
  "anya-e0606n:240": "Iti jīvitamadagabbita matirujjhita kusalo;",
  "anya-e0606n:241": "Amariṃ bhavagahaṇe caraṃ ahaṃ appaṭisaraṇe,",
  "anya-e0606n:242": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:243": "48.",
  "anya-e0606n:244": "Parahiṃsana dhanamosana paradārika nirato,",
  "anya-e0606n:245": "Dharanīpati gahito bahu vadhabandhana nihato;",
  "anya-e0606n:246": "Amariṃ bhavagahaṇe caraṃ ahaṃ appaṭisaraṇe,",
  "anya-e0606n:247": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:248": "49.",
  "anya-e0606n:249": "Narakodaka patito giritarumatthaka galito,",
  "anya-e0606n:250": "Migavāḷaka gahito visadhara jātihi ḍasito;",
  "anya-e0606n:251": "Amariṃ bhavagahaṇe caraṃ ahaṃ appaṭisaraṇe,",
  "anya-e0606n:252": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:253": "50.",
  "anya-e0606n:254": "Abhicāraka vidhikopita nisicāraka gahito,",
  "anya-e0606n:255": "Savisodana sahasādana pabhutīhi ca khalito;",
  "anya-e0606n:256": "Amariṃ bhavagahaṇe caraṃ ahaṃ appaṭisaraṇe,",
  "anya-e0606n:257": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:258": "51.",
  "anya-e0606n:259": "Sayamevaca sajanopari kupito mativiyuto,",
  "anya-e0606n:260": "Savisādana galakantana pabhutāmita khalito;",
  "anya-e0606n:261": "Amariṃ bhavagahaṇe caraṃ ahaṃ appaṭisaraṇe,",
  "anya-e0606n:262": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:263": "52.",
  "anya-e0606n:264": "Pitupūjana niratāsaya sukhito pitudayito,",
  "anya-e0606n:265": "Maraṇenaca pituno bhusaṃ anusocana nirato;",
  "anya-e0606n:266": "Parudiṃ ciraṃ atidussaha kasire bhavagahaṇe,",
  "anya-e0606n:267": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:268": "53.",
  "anya-e0606n:269": "Samupaṭṭhiya janikaṃ nijaṃ abhivādana paramo,",
  "anya-e0606n:270": "Maraṇenaca janikāyanusaritā guṇamahimaṃ;",
  "anya-e0606n:271": "Parudiṃ ciraṃ atidussaha kasire bhavagahaṇe,",
  "anya-e0606n:272": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:273": "54.",
  "anya-e0606n:274": "Gurudevata patimānana paramo piyasuvaco,",
  "anya-e0606n:275": "Samupāsitacaraṇo gurumaraṇenatidukhito;",
  "anya-e0606n:276": "Parudiṃ ciraṃ atidussaha kasire bhavagahaṇe,",
  "anya-e0606n:277": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:278": "55.",
  "anya-e0606n:279": "Gharamedhitaṃ upagomitavibhavo ratibahulo,",
  "anya-e0606n:280": "Maraṇe piyabhariyāsutaduhitū’natikaruṇaṃ;",
  "anya-e0606n:281": "Parudiṃ ciraṃ atidussaha kasire bhavagahaṇe,",
  "anya-e0606n:282": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:283": "56.",
  "anya-e0606n:284": "Piyapubbaja sahajānuja bhagiṇidvaya maraṇe,",
  "anya-e0606n:285": "Nija bandhava-sakha-sissaka maraṇe pyatikaruṇaṃ;",
  "anya-e0606n:286": "Parudiṃ ciraṃ atidussaha kasire bhavagahaṇe,",
  "anya-e0606n:287": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:288": "57.",
  "anya-e0606n:289": "Jagatīpati gahaṇā ripujanatakkara haraṇā,",
  "anya-e0606n:290": "Saritodaka vahaṇā puthujalitānala dahaṇā;",
  "anya-e0606n:291": "Parudiṃ hatavibhavo ciramiha duggatigahaṇe,",
  "anya-e0606n:292": "Saraṇaṃ bhava bhagavaṃ mama bhavanīradhi taraṇe.",
  "anya-e0606n:293": "58.",
  "anya-e0606n:294": "Atiduggama visamākula bhavasaṅkaṭa patite,",
  "anya-e0606n:295": "Byasanaṃ ciramiti dussahaṃ anubhūyapi vimitaṃ;",
  "anya-e0606n:296": "Na jahe sukhalavavañcitahadayo bhavatasinaṃ,",
  "anya-e0606n:297": "Tamapākuru karuṇānidhi tasinaṃ mama kasiṇaṃ.",
  "anya-e0606n:298": "59.",
  "anya-e0606n:299": "Jananāvadhi maraṇaṃ viya maraṇāvadhi jananaṃ,",
  "anya-e0606n:300": "Ubhayenapi bhayamevahi bhavato mama niyataṃ;",
  "anya-e0606n:301": "Sivamevaca jananāvadhi maraṇāvadhi rahitaṃ,",
  "anya-e0606n:302": "Dada me sivaṃ amatandada taṃ anāsava bhagavaṃ.",
  "anya-e0606n:303": "60.",
  "anya-e0606n:304": "Ciradikkhitamapi me manaṃ anivārita tasinaṃ,",
  "anya-e0606n:305": "Bhavato bhava ratipīlitaṃ ahaho kalighaṭitaṃ!",
  "anya-e0606n:306": "Tamato pita bhayato mamamava me bhava saraṇaṃ,",
  "anya-e0606n:307": "Bhagavaṃ paṭisaraṇaṃ mama bhavanīradhi taraṇaṃ."
}