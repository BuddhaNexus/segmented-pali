{
  "anya-e0603n2:0": "Uṇṇālomikanātha vandanā",
  "anya-e0603n2:1": "Uṇṇālodhikanāthassa ,",
  "anya-e0603n2:2": "Uṇṇāya bhamukantare;",
  "anya-e0603n2:3": "Vajirāviya sobhanti,",
  "anya-e0603n2:4": "Nikkhanti yo supaṇḍarā.",
  "anya-e0603n2:5": "Vajiragghanakāyassa ,",
  "anya-e0603n2:6": "Nāthassa dehato subhā;",
  "anya-e0603n2:7": "Vajireyyā niccharanti;",
  "anya-e0603n2:8": "Vijjūva gagaṇantare.",
  "anya-e0603n2:9": "Rinindamandahāsena,",
  "anya-e0603n2:10": "Dāṭhāya niccharā pabhā;",
  "anya-e0603n2:11": "Vipphūrisā disāsabbā,",
  "anya-e0603n2:12": "Vajirāyanti paṇḍarā."
}