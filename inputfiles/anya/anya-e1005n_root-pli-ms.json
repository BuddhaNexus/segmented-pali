{
  "anya-e1005n:0": " Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1005n:1": "Lokanīti",
  "anya-e1005n:2": "1. Paṇḍitakaṇḍo",
  "anya-e1005n:3": "1.",
  "anya-e1005n:4": " Lokanītiṃ pavakkhāmi,",
  "anya-e1005n:5": "Nānāsatthasamuddhaṭaṃ;",
  "anya-e1005n:6": "Māgadheneva saṅkhepaṃ,",
  "anya-e1005n:7": "Vanditvā ratanatthayaṃ.",
  "anya-e1005n:8": "2.",
  "anya-e1005n:9": " Nītiloke purisassa sāro,",
  "anya-e1005n:10": "Mātā pitā ācariyoca mitto;",
  "anya-e1005n:11": "Tasmā hi nītiṃ puriso vijaññā,",
  "anya-e1005n:12": "Ñāṇī mahā hoti bahussutoca.",
  "anya-e1005n:13": "3.",
  "anya-e1005n:14": " Alasassa kuto sippaṃ,",
  "anya-e1005n:15": "Asippassa kuto dhanaṃ;",
  "anya-e1005n:16": "Adhanassa kuto mittaṃ,",
  "anya-e1005n:17": "Amittassa kuto sukhaṃ;",
  "anya-e1005n:18": "Asukhassa kuto puññaṃ,",
  "anya-e1005n:19": "Apuññassa kuto varaṃ.",
  "anya-e1005n:20": "4.",
  "anya-e1005n:21": " Sippā samaṃ dhanaṃ natthi,",
  "anya-e1005n:22": "Sippaṃ corā nagaṇhare;",
  "anya-e1005n:23": "Idha loke sippaṃ mittaṃ,",
  "anya-e1005n:24": "Paraloke sukhāvahaṃ.",
  "anya-e1005n:25": "5.",
  "anya-e1005n:26": " Appakaṃ nātimaññeyya,",
  "anya-e1005n:27": "Citte sutaṃ nidhāpaye;",
  "anya-e1005n:28": "Vammikodakabindūva,",
  "anya-e1005n:29": "Cirena paripūrati.",
  "anya-e1005n:30": "6.",
  "anya-e1005n:31": " Khuddoti nātimaññeyya,",
  "anya-e1005n:32": "Vijjaṃ vā sippameva vā;",
  "anya-e1005n:33": "Ekampi pariyodātaṃ,",
  "anya-e1005n:34": "Jīvitakappakāraṇaṃ.",
  "anya-e1005n:35": "7.",
  "anya-e1005n:36": " Sele sele na māṇikaṃ,",
  "anya-e1005n:37": "Gaje gaje na muttikaṃ;",
  "anya-e1005n:38": "Vane vane na candanaṃ,",
  "anya-e1005n:39": "Ṭhāne ṭhāne na paṇḍitaṃ.",
  "anya-e1005n:40": "8.",
  "anya-e1005n:41": " Paṇḍito sutasampanno,",
  "anya-e1005n:42": "Yattha atthīti ce suto;",
  "anya-e1005n:43": "Mahussāhena taṃ ṭhānaṃ,",
  "anya-e1005n:44": "Gantabbaṃva sutesinā.",
  "anya-e1005n:45": "9.",
  "anya-e1005n:46": " Sine sippaṃ sine dhanaṃ,",
  "anya-e1005n:47": "Sine pabbatamāruhaṃ;",
  "anya-e1005n:48": "Sine kāmassa kodhassa,",
  "anya-e1005n:49": "Ime pañca sine sine.",
  "anya-e1005n:50": "10.",
  "anya-e1005n:51": " Suti sammuti saṅkhyāca,",
  "anya-e1005n:52": "Yogā nīti visesakā;",
  "anya-e1005n:53": "Gandhabbā gaṇikā ceva,",
  "anya-e1005n:54": "Dhanu bedā ca pūraṇā.",
  "anya-e1005n:55": "11.",
  "anya-e1005n:56": " Tikicchā itihāsā ca,",
  "anya-e1005n:57": "Joti māyā ca chandati;",
  "anya-e1005n:58": "Ketu mantā ca saddā ca,",
  "anya-e1005n:59": "Sippāṭṭhārasakā ime.",
  "anya-e1005n:60": "12.",
  "anya-e1005n:61": " Apuṭṭho paṇḍito bherī,",
  "anya-e1005n:62": "Pajjunno me hoti pucchito;",
  "anya-e1005n:63": "Bālo puṭṭho apuṭṭhopi,",
  "anya-e1005n:64": "Bahumpi bhaṇate sadā.",
  "anya-e1005n:65": "13.",
  "anya-e1005n:66": " Potthakesu ca yaṃ sippaṃ,",
  "anya-e1005n:67": "Parahatthesu yaṃ dhanaṃ;",
  "anya-e1005n:68": "Yathākicce samuppanne,",
  "anya-e1005n:69": "Na taṃ sippaṃ na taṃ dhanaṃ.",
  "anya-e1005n:70": "14.",
  "anya-e1005n:71": " Jalappamāṇaṃ kumuddanālaṃ,",
  "anya-e1005n:72": "Kulappamāṇaṃ vinayo pamāṇaṃ;",
  "anya-e1005n:73": "Byattippamāṇaṃ kathītavākyaṃ,",
  "anya-e1005n:74": "Pathaviyā pamāṇaṃ tiṇa milātaṃ.",
  "anya-e1005n:75": "15.",
  "anya-e1005n:76": " Appassuto sutaṃ appaṃ,",
  "anya-e1005n:77": "Bahuṃ maññati mānavā;",
  "anya-e1005n:78": "Sindhūdakaṃ apassanto,",
  "anya-e1005n:79": "Kūpe toyaṃva maṇḍuko.",
  "anya-e1005n:80": "16.",
  "anya-e1005n:81": " Pathamaṃ parājaye sippaṃ,",
  "anya-e1005n:82": "Dutiyaṃ parājaye dhanaṃ;",
  "anya-e1005n:83": "Tatiyaṃ parājaye dhammaṃ,",
  "anya-e1005n:84": "Catutthaṃ kiṃ karissati.",
  "anya-e1005n:85": "17.",
  "anya-e1005n:86": " Byatta putra kimalaso,",
  "anya-e1005n:87": "Abyatto bhārahārako;",
  "anya-e1005n:88": "Byattako pūjito loke,",
  "anya-e1005n:89": "Byatta putra dine dine.",
  "anya-e1005n:90": "18.",
  "anya-e1005n:91": " Mātā verī pitā satru,",
  "anya-e1005n:92": "Kena bāle na sikkhitā;",
  "anya-e1005n:93": "Sabhāmajjhe na sobhanti,",
  "anya-e1005n:94": "Haṃsamajjhe bakoyathā.",
  "anya-e1005n:95": "19.",
  "anya-e1005n:96": " Kaṇṭakaṃ giri ko tikkhati,",
  "anya-e1005n:97": "Ko añjanaṃ migakkhikaṃ;",
  "anya-e1005n:98": "Uppathaṃ pallale ko sugandhaṃ,",
  "anya-e1005n:99": "Kula-putta-rūpo ko pavattati;",
  "anya-e1005n:100": "Sāmaṃ-bhāvo.",
  "anya-e1005n:101": "20.",
  "anya-e1005n:102": " Na rasaṃ akotambulaṃ,",
  "anya-e1005n:103": "Adhanassa, laṅkatampi;",
  "anya-e1005n:104": "Alonakantu byañjanaṃ,",
  "anya-e1005n:105": "Byākaraṇaṃ asippassa.",
  "anya-e1005n:106": "21.",
  "anya-e1005n:107": " Sussusā sutasampanno,",
  "anya-e1005n:108": "Sutāpaññāya pavaḍḍhati;",
  "anya-e1005n:109": "Paññāya atthaṃ jānāti,",
  "anya-e1005n:110": "Ñāto attho sukhāvaho.",
  "anya-e1005n:111": "22.",
  "anya-e1005n:112": " Bhojanaṃ methunaṃ niddā,",
  "anya-e1005n:113": "Goṇe posepi vijjati;",
  "anya-e1005n:114": "Vijjā viseso posassa,",
  "anya-e1005n:115": "Taṃ hīno gosamo bhave.",
  "anya-e1005n:116": "23.",
  "anya-e1005n:117": " Natthi vijjāsamaṃmittaṃ,",
  "anya-e1005n:118": "Naca byādhisamo ripu;",
  "anya-e1005n:119": "Naca attasamaṃ pemaṃ,",
  "anya-e1005n:120": "Naca kammasamaṃ balaṃ.",
  "anya-e1005n:121": "24.",
  "anya-e1005n:122": " Haṃso majjhe na kākānaṃ,",
  "anya-e1005n:123": "Sīho gunnaṃ na sobhate;",
  "anya-e1005n:124": "Gadrabhamajjhe turaṅgo,",
  "anya-e1005n:125": "Bālamajjhe ca paṇḍito.",
  "anya-e1005n:126": "25.",
  "anya-e1005n:127": " Yāvajīvampi ce bālo,",
  "anya-e1005n:128": "Paṇḍitaṃ payirupāsati;",
  "anya-e1005n:129": "Na so dhammaṃ vijānāti,",
  "anya-e1005n:130": "Dabbi sūparasaṃ yathā.",
  "anya-e1005n:131": "26.",
  "anya-e1005n:132": " Muhuttamapi ce viññū,",
  "anya-e1005n:133": "Paṇḍitaṃ payirupāsati;",
  "anya-e1005n:134": "Khippaṃ dhammaṃ vijānāti,",
  "anya-e1005n:135": "Jivhā sūparasaṃ yathā.",
  "anya-e1005n:136": "27.",
  "anya-e1005n:137": " Vinā satthaṃ na gaccheyya,",
  "anya-e1005n:138": "Sūro saṅgāmabhūmiyaṃ;",
  "anya-e1005n:139": "Paṇḍitvāddhagū vāṇijo,",
  "anya-e1005n:140": "Videsagamano tathā.",
  "anya-e1005n:141": "28.",
  "anya-e1005n:142": " Dhananāsaṃ manotāpaṃ,",
  "anya-e1005n:143": "Ghare duccaritāni ca;",
  "anya-e1005n:144": "Vañcanañca avamānaṃ,",
  "anya-e1005n:145": "Paṇḍito na pakāsaye.",
  "anya-e1005n:146": "29.",
  "anya-e1005n:147": " Pattānurūpakaṃ vākyaṃ,",
  "anya-e1005n:148": "Sabhāvarūpakaṃ piyaṃ;",
  "anya-e1005n:149": "Attānurūpakaṃ kodhaṃ,",
  "anya-e1005n:150": "Yo jānāti sa paṇḍito.",
  "anya-e1005n:151": "30.",
  "anya-e1005n:152": " A-dhanassa rasaṃ khādā,",
  "anya-e1005n:153": "A-balassa hathā narā;",
  "anya-e1005n:154": "A-paññassa vākya-kathā,",
  "anya-e1005n:155": "Ummattaka-samā ime.",
  "anya-e1005n:156": "31.",
  "anya-e1005n:157": " Anavhāyaṃ gamayanto,",
  "anya-e1005n:158": "A-pucchā bahu-bhāsako;",
  "anya-e1005n:159": "Atta-guṇaṃ pakāsento,",
  "anya-e1005n:160": "Ti-vidhaṃ hīna-lakkhaṇaṃ.",
  "anya-e1005n:161": "32.",
  "anya-e1005n:162": " Appa-rūpo bahuṃ bhāso,",
  "anya-e1005n:163": "Appa-pañño pakāsito;",
  "anya-e1005n:164": "Appa-pūro ghaṭo khobhe,",
  "anya-e1005n:165": "Appa-khīrā gāvī cathe.",
  "anya-e1005n:166": "33.",
  "anya-e1005n:167": " Maṇḍūkepi ukre sīhe,",
  "anya-e1005n:168": "Kākaggahe piye piye;",
  "anya-e1005n:169": "A-paṇḍīpi paṇḍī hutvā,",
  "anya-e1005n:170": "Dhīrā pucche vaye vaye.",
  "anya-e1005n:171": "34.",
  "anya-e1005n:172": " Maṇḍūkepi ukre sīhe,",
  "anya-e1005n:173": "Sūkarepi uhe dīpe;",
  "anya-e1005n:174": "Biḷāre sadise byagghe,",
  "anya-e1005n:175": "Sabba dhīre sippa-same.",
  "anya-e1005n:176": "35.",
  "anya-e1005n:177": " Na titti rājā dhanamhi,",
  "anya-e1005n:178": "Paṇḍitopi su-bhāsite;",
  "anya-e1005n:179": "Cakkhuṃpi piya-dassane,",
  "anya-e1005n:180": "Jale sāgaro na titti.",
  "anya-e1005n:181": "36.",
  "anya-e1005n:182": " Rūpa-yobbana-sampannā,",
  "anya-e1005n:183": "Visātha-kutha-sambhavā;",
  "anya-e1005n:184": "Vijjā-hīnā na sobhanti,",
  "anya-e1005n:185": "Nigandhā iva kiṃsukā.",
  "anya-e1005n:186": "37.",
  "anya-e1005n:187": " Hīne putto rājāmacco,",
  "anya-e1005n:188": "Bāla-putto ca paṇḍito;",
  "anya-e1005n:189": "A-dhanassa dhanaṃ bahu,",
  "anya-e1005n:190": "Purisānaṃ na maññatha.",
  "anya-e1005n:191": "38.",
  "anya-e1005n:192": " Yo sippa-lobhena,",
  "anya-e1005n:193": "Bahuṃ gaṇhāti taṃ sippaṃ;",
  "anya-e1005n:194": "Mūgova supinaṃ passaṃ,",
  "anya-e1005n:195": "Kathetumpi na ussahe.",
  "anya-e1005n:196": "39.",
  "anya-e1005n:197": " Bhijjetuṃ kumbhakāro,",
  "anya-e1005n:198": "Sobhetuṃ kumbha ghaṭṭati;",
  "anya-e1005n:199": "Na khipituṃ apāyesu,",
  "anya-e1005n:200": "Sissānaṃ vuḍḍhi-kāraṇā.",
  "anya-e1005n:201": "40.",
  "anya-e1005n:202": " Taggarañca palāsena,",
  "anya-e1005n:203": "Yo naro upanayhati;",
  "anya-e1005n:204": "Pattāpi surabhi vāyanti,",
  "anya-e1005n:205": "Evaṃ dhīrūpasevanā.",
  "anya-e1005n:206": "Paṇḍitakaṇḍo niṭṭhito.",
  "anya-e1005n:207": "Sujanakaṇḍo",
  "anya-e1005n:208": "41.",
  "anya-e1005n:209": " Sabbhireva samāsetha,",
  "anya-e1005n:210": "Sabbhi kubbetha santhavaṃ;",
  "anya-e1005n:211": "Sataṃ saddhammamaññāya,",
  "anya-e1005n:212": "Seyyo hoti na pāpiyo.",
  "anya-e1005n:213": "42.",
  "anya-e1005n:214": " Caja dujjana saṃsaggaṃ,",
  "anya-e1005n:215": "Bhaja sādhu samāgamaṃ;",
  "anya-e1005n:216": "Kara puññamahorattiṃ,",
  "anya-e1005n:217": "Sara niccamaniccataṃ.",
  "anya-e1005n:218": "43.",
  "anya-e1005n:219": " Yathā udumbarapakkā,",
  "anya-e1005n:220": "Bahirattakamevaca;",
  "anya-e1005n:221": "Anto kimīhi sampuṇṇā,",
  "anya-e1005n:222": "Evaṃ dujjanahaddayā.",
  "anya-e1005n:223": "44.",
  "anya-e1005n:224": " Yathāpi panasāpakkā,",
  "anya-e1005n:225": "Bahi kaṇḍakameva ca;",
  "anya-e1005n:226": "Anto amatasampannā,",
  "anya-e1005n:227": "Evaṃ sujanahadayā.",
  "anya-e1005n:228": "45.",
  "anya-e1005n:229": " Sukkhopi candanataru na jahāti gandhaṃ,",
  "anya-e1005n:230": "Nāgo gato naramukhe na jahāti līḷaṃ;",
  "anya-e1005n:231": "Yantāgato madhurasaṃ na jahāti ucchu,",
  "anya-e1005n:232": "Dukkhopi paṇḍitajano na jahāti dhammaṃ.",
  "anya-e1005n:233": "46.",
  "anya-e1005n:234": " Sīho nāma jighacchāpi,",
  "anya-e1005n:235": "Paṇṇādīni na khādati;",
  "anya-e1005n:236": "Sīho nāma kisocāpi,",
  "anya-e1005n:237": "Nāgamaṃsaṃ na khādati.",
  "anya-e1005n:238": "47.",
  "anya-e1005n:239": " Kula-jāto kula-putto,",
  "anya-e1005n:240": "Kula-vaṃsa-surakkhato;",
  "anya-e1005n:241": "Attanā dukkha-pattopi,",
  "anya-e1005n:242": "Hīna-kammaṃ na kāraye.",
  "anya-e1005n:243": "48.",
  "anya-e1005n:244": " Candanaṃ sītalaṃ loke,",
  "anya-e1005n:245": "Tato candaṃva sītalaṃ;",
  "anya-e1005n:246": "Canda-candanasītamhā,",
  "anya-e1005n:247": "Sādhu vākyaṃ subhāsitaṃ.",
  "anya-e1005n:248": "49.",
  "anya-e1005n:249": " Udeyya bhāṇu pacchime,",
  "anya-e1005n:250": "Merurājā nameyyapi;",
  "anya-e1005n:251": "Sītalā narakaggipi,",
  "anya-e1005n:252": "Pabbatagge ca uppalaṃ.",
  "anya-e1005n:253": "Vikase na viparītaṃ,",
  "anya-e1005n:254": "Sādhuvāyyaṃ kudācanaṃ.",
  "anya-e1005n:255": "50.",
  "anya-e1005n:256": " Sukhā rukkhassa chāyāva,",
  "anya-e1005n:257": "Tato ñāti mātā pitu;",
  "anya-e1005n:258": "Tato ācariyo rañño,",
  "anya-e1005n:259": "Tato buddhassanekadhā.",
  "anya-e1005n:260": "51.",
  "anya-e1005n:261": " Bhamarā pupphamicchanti,",
  "anya-e1005n:262": "Guṇamicchanti sujanā;",
  "anya-e1005n:263": "Makkhikā pūtimicchanti,",
  "anya-e1005n:264": "Dosamicchanti dujjanā.",
  "anya-e1005n:265": "52.",
  "anya-e1005n:266": " Mātāhīnassa dubbhāsā,",
  "anya-e1005n:267": "Pitāhīnassa dukriyā;",
  "anya-e1005n:268": "Ubho mātā pitā hīnā,",
  "anya-e1005n:269": "Dubbhasāca dukīriyā.",
  "anya-e1005n:270": "53.",
  "anya-e1005n:271": " Mātā seṭṭhassa subhāsā,",
  "anya-e1005n:272": "Pitā seṭṭhassa sukriyā;",
  "anya-e1005n:273": "Ubho mātā pitā seṭṭhā,",
  "anya-e1005n:274": "Subhāsāca sukīriyā.",
  "anya-e1005n:275": "54.",
  "anya-e1005n:276": " Saṅgāme sūra-micchanti,",
  "anya-e1005n:277": "Mantīsu akūtūhalaṃ;",
  "anya-e1005n:278": "Piyañca annapānesu,",
  "anya-e1005n:279": "Atthe jāte ca paṇḍitaṃ.",
  "anya-e1005n:280": "55.",
  "anya-e1005n:281": " Sunakho sunakhaṃ disvā,",
  "anya-e1005n:282": "Dantaṃ dasseti hiṃsituṃ;",
  "anya-e1005n:283": "Dujjano sujanaṃ disvā,",
  "anya-e1005n:284": "Rosayaṃ hiṃsamicchati.",
  "anya-e1005n:285": "56.",
  "anya-e1005n:286": " Mā ca vegena kiccāni,",
  "anya-e1005n:287": "Karosi kārāpesi vā;",
  "anya-e1005n:288": "Sahasā kāritaṃ kammaṃ,",
  "anya-e1005n:289": "Mando pacchānutappati.",
  "anya-e1005n:290": "57.",
  "anya-e1005n:291": " Kodhaṃ vihitvā na kadācinasoce,",
  "anya-e1005n:292": "Makkhappahānaṃ isayo avaṇṇayuṃ;",
  "anya-e1005n:293": "Sabbesa phārusa-vacaṃ khametha,",
  "anya-e1005n:294": "Etaṃ khantiṃ uttamamāhu santo.",
  "anya-e1005n:295": "58.",
  "anya-e1005n:296": " Dukkho nivāso sambādhe,",
  "anya-e1005n:297": "Ṭhāne asucisaṅkate;",
  "anya-e1005n:298": "Tato arimhi appiye,",
  "anya-e1005n:299": "Tatopi akataññunā.",
  "anya-e1005n:300": "59.",
  "anya-e1005n:301": " Ovādeyyānusāseyya,",
  "anya-e1005n:302": "Gāpakā ca nivāraye;",
  "anya-e1005n:303": "Satañhi so piyo hoti,",
  "anya-e1005n:304": "Asataṃ hoti appiyo.",
  "anya-e1005n:305": "60.",
  "anya-e1005n:306": " Uttamattanivātena,",
  "anya-e1005n:307": "Sūraṃ bhedena nijjaye;",
  "anya-e1005n:308": "Nīcaṃ appaka dānena,",
  "anya-e1005n:309": "Vīriyena samaṃ jaye.",
  "anya-e1005n:310": "61.",
  "anya-e1005n:311": " Na visaṃ visamiccāhu,",
  "anya-e1005n:312": "Dhanaṃ saṅghassa uccate;",
  "anya-e1005n:313": "Visaṃ ekaṃva hanati,",
  "anya-e1005n:314": "Sabbaṃ saṅghassa santakaṃ.",
  "anya-e1005n:315": "62.",
  "anya-e1005n:316": " Javane bhadraṃ jānanti,",
  "anya-e1005n:317": "Baliddañca vāhenā;",
  "anya-e1005n:318": "Duhena dhenuṃ jānanti,",
  "anya-e1005n:319": "Bhāsamānena paṇḍitaṃ.",
  "anya-e1005n:320": "63.",
  "anya-e1005n:321": " Dhanamappampi sādhūnaṃ,",
  "anya-e1005n:322": "Kūpe vāriva nissayo;",
  "anya-e1005n:323": "Bahuṃ api asādhūnaṃ,",
  "anya-e1005n:324": "Naca vārīva aṇṇave.",
  "anya-e1005n:325": "64.",
  "anya-e1005n:326": " Najjo pivanti no āpaṃ,",
  "anya-e1005n:327": "Rukkhā khādanti no phalaṃ;",
  "anya-e1005n:328": "Vassanti kvaci no meghā,",
  "anya-e1005n:329": "Paratthāya sataṃ dhanaṃ.",
  "anya-e1005n:330": "65.",
  "anya-e1005n:331": " Apattheyyaṃ na pattheyya,",
  "anya-e1005n:332": "A cinteyyaṃ na cintaye;",
  "anya-e1005n:333": "Dhammameva sucinteyya,",
  "anya-e1005n:334": "Kālaṃ moghaṃ na accaye.",
  "anya-e1005n:335": "66.",
  "anya-e1005n:336": " Acintitampi bhavati,",
  "anya-e1005n:337": "Cintitampi vinassati;",
  "anya-e1005n:338": "Na hi cintāmayā bhogā,",
  "anya-e1005n:339": "Itthiyā purisassavā.",
  "anya-e1005n:340": "67.",
  "anya-e1005n:341": " Asantassa piyo hoti,",
  "anya-e1005n:342": "Sante na kurute piyaṃ;",
  "anya-e1005n:343": "Asataṃ dhammaṃ roceti,",
  "anya-e1005n:344": "Taṃ parābhavato mukhaṃ.",
  "anya-e1005n:345": "Sujanakaṇḍo niṭṭhito.",
  "anya-e1005n:346": "4. Bāladujjanakaṇḍo",
  "anya-e1005n:347": "68.",
  "anya-e1005n:348": " Atippiyo na kātabbo,",
  "anya-e1005n:349": "Khalo kotūhalaṃ karo;",
  "anya-e1005n:350": "Sirasā vahyamānopi,",
  "anya-e1005n:351": "Aḍḍhapūro ghaṭo yathā.",
  "anya-e1005n:352": "69.",
  "anya-e1005n:353": " Sappo duṭṭho khalo duṭṭho,",
  "anya-e1005n:354": "Sappo duṭṭhataro khalo;",
  "anya-e1005n:355": "Mantosadhehi so sappo,",
  "anya-e1005n:356": "Khalo kenupasammati.",
  "anya-e1005n:357": "70.",
  "anya-e1005n:358": " Yo bālo maññati bālyaṃ,",
  "anya-e1005n:359": "Paṇḍito vāpi tena so;",
  "anya-e1005n:360": "Bālova paṇḍitamānī,",
  "anya-e1005n:361": "So ve bāloti vuccati.",
  "anya-e1005n:362": "71.",
  "anya-e1005n:363": " Madhuṃva maññatī bālo,",
  "anya-e1005n:364": "Yāva pāpaṃ na paccati;",
  "anya-e1005n:365": "Yadāca paccatī pāpaṃ,",
  "anya-e1005n:366": "Atha dukkhaṃ nigacchati.",
  "anya-e1005n:367": "72.",
  "anya-e1005n:368": " Na sādhu balavā bālo,",
  "anya-e1005n:369": "Sahasā vindate dhanaṃ;",
  "anya-e1005n:370": "Kāyassa bhedā duppañño,",
  "anya-e1005n:371": "Nirayaṃ sopapajjati.",
  "anya-e1005n:372": "73.",
  "anya-e1005n:373": " Ghare duṭṭho ca mūsīko ca,",
  "anya-e1005n:374": "Vane duṭṭho ca vānaro;",
  "anya-e1005n:375": "Sakuṇe ca duṭṭho kāko,",
  "anya-e1005n:376": "Nare duṭṭhoca brāhmaṇo.",
  "anya-e1005n:377": "74.",
  "anya-e1005n:378": " Dīghā jāgarato ratti,",
  "anya-e1005n:379": "Dīghaṃ santassa yojanaṃ;",
  "anya-e1005n:380": "Dīgho bālāna saṃsāro,",
  "anya-e1005n:381": "Saddhammaṃ a-vijānataṃ.",
  "anya-e1005n:382": "75.",
  "anya-e1005n:383": " Tila mattaṃ paresaṃva,",
  "anya-e1005n:384": "Appa dosañca passati;",
  "anya-e1005n:385": "Nāḷikerampi sadosaṃ,",
  "anya-e1005n:386": "Khala-jāto na passati.",
  "anya-e1005n:387": "76.",
  "anya-e1005n:388": " Nattadosaṃ pare jaññā,",
  "anya-e1005n:389": "Jaññā dosaṃ parassatu;",
  "anya-e1005n:390": "Guyhe kummo aṅgāni,",
  "anya-e1005n:391": "Para dosañca lakkhaye.",
  "anya-e1005n:392": "77.",
  "anya-e1005n:393": " Paṇḍitassa pasaṃsāya,",
  "anya-e1005n:394": "Daṇḍo bālena dīyate;",
  "anya-e1005n:395": "Paṇḍito paṇḍiteneva,",
  "anya-e1005n:396": "Vaṇṇitova suvaṇṇito.",
  "anya-e1005n:397": "78.",
  "anya-e1005n:398": " Luddhaṃ atthena gaṇheyya,",
  "anya-e1005n:399": "Thaddhaṃ añjali kammunā;",
  "anya-e1005n:400": "Chandānuvattiyā mūḷhaṃ,",
  "anya-e1005n:401": "Yathābhūtena paṇḍitaṃ.",
  "anya-e1005n:402": "Bāladujjanakaṇḍo niṭṭhito.",
  "anya-e1005n:403": "4. Mittakaṇḍo",
  "anya-e1005n:404": "79.",
  "anya-e1005n:405": " Hitakāro paro bandhu,",
  "anya-e1005n:406": "Bandhūpi ahito paro;",
  "anya-e1005n:407": "A hito dehajo byādhi,",
  "anya-e1005n:408": "Hitaṃ araññamosadhaṃ.",
  "anya-e1005n:409": "80.",
  "anya-e1005n:410": " Parokkhe guṇa-hantāraṃ,",
  "anya-e1005n:411": "Paccakkhe piyavādinaṃ;",
  "anya-e1005n:412": "Vajjeyya tādisaṃ mittaṃ,",
  "anya-e1005n:413": "Visakumbhe yathā madhuṃ.",
  "anya-e1005n:414": "81.",
  "anya-e1005n:415": " Dhanahīne caje mitto,",
  "anya-e1005n:416": "Puttadārā sahodarā;",
  "anya-e1005n:417": "Dhanavantaṃva sevanti,",
  "anya-e1005n:418": "Dhanaṃ loke mahā sakhā.",
  "anya-e1005n:419": "82.",
  "anya-e1005n:420": " Jāneyya pesena bhaccaṃ,",
  "anya-e1005n:421": "Bandhuṃ vāpi bhayāgate;",
  "anya-e1005n:422": "Appakāsu tathā mittaṃ,",
  "anya-e1005n:423": "Dārañca vibhavakkhaye.",
  "anya-e1005n:424": "83.",
  "anya-e1005n:425": " So bandhu yo hite yutto,",
  "anya-e1005n:426": "Pitaro honti posako;",
  "anya-e1005n:427": "Taṃ mittaṃ yattha vissāso,",
  "anya-e1005n:428": "Sā bhariyā ca yassa nibbūti.",
  "anya-e1005n:429": "84.",
  "anya-e1005n:430": " Na vissase a-vissattaṃ,",
  "anya-e1005n:431": "Mittañcāpi na vissase;",
  "anya-e1005n:432": "Kadāci kupito mito,",
  "anya-e1005n:433": "Sabbaṃ dosaṃ pakāsaye.",
  "anya-e1005n:434": "85.",
  "anya-e1005n:435": " Sakiṃ duṭṭhañca yo mittaṃ,",
  "anya-e1005n:436": "Puna sandhitumicchati;",
  "anya-e1005n:437": "So maccuṃ upagaṇhāti,",
  "anya-e1005n:438": "Gabbhamassatarī yathā.",
  "anya-e1005n:439": "86.",
  "anya-e1005n:440": " Vahe a-mittaṃ khandhena,",
  "anya-e1005n:441": "Yāva kālo anāgato;",
  "anya-e1005n:442": "Tameva āgate kāle,",
  "anya-e1005n:443": "Sele bhinde ghaṭaṃ iva.",
  "anya-e1005n:444": "87.",
  "anya-e1005n:445": " Iṇaseso aggiseso,",
  "anya-e1005n:446": "Satruseso tatheva ca;",
  "anya-e1005n:447": "Punappunaṃ vivaḍḍhanti,",
  "anya-e1005n:448": "Tasmā sesaṃ na kāraye.",
  "anya-e1005n:449": "88.",
  "anya-e1005n:450": " Padumaṃva mukhaṃ yassa,",
  "anya-e1005n:451": "Vācā candana sītalā;",
  "anya-e1005n:452": "Tādisaṃ nopaseveyya,",
  "anya-e1005n:453": "Hadayetu halāhalaṃ.",
  "anya-e1005n:454": "89.",
  "anya-e1005n:455": " Na seve pharusaṃ sāmiṃ,",
  "anya-e1005n:456": "Naca seveyya macchariṃ;",
  "anya-e1005n:457": "Tato apaggaṇhaṃ sāmiṃ,",
  "anya-e1005n:458": "Neva niggahitaṃ tato.",
  "anya-e1005n:459": "90.",
  "anya-e1005n:460": " Siṅgī paññāsa hatthena,",
  "anya-e1005n:461": "Vajje satena vājinaṃ;",
  "anya-e1005n:462": "Hatthiṃ dantiṃ sahassena,",
  "anya-e1005n:463": "Desa cāgena dujjanaṃ.",
  "anya-e1005n:464": "91.",
  "anya-e1005n:465": " Kudesañca kumittañca,",
  "anya-e1005n:466": "Kukulañca kubandhavaṃ;",
  "anya-e1005n:467": "Kudārañca kudāsañca,",
  "anya-e1005n:468": "Dūrato parivajjaye.",
  "anya-e1005n:469": "92.",
  "anya-e1005n:470": " Rogāture ca dubbhikkhe,",
  "anya-e1005n:471": "Byasane sattu viggahe;",
  "anya-e1005n:472": "Rājadvāre susāne ca,",
  "anya-e1005n:473": "Ye tiṭṭhanti sumittakā.",
  "anya-e1005n:474": "93.",
  "anya-e1005n:475": " Sītavāco bahumitto,",
  "anya-e1005n:476": "Pharuso appamittako;",
  "anya-e1005n:477": "Upamaṃ ettha ñātabbā,",
  "anya-e1005n:478": "Canda-sūriya-rājūnaṃ.",
  "anya-e1005n:479": "Mittakaṇḍo niṭṭhito.",
  "anya-e1005n:480": "5. Itthikaṇḍo",
  "anya-e1005n:481": "94.",
  "anya-e1005n:482": " Kokilānaṃ saddaṃ rūpaṃ,",
  "anya-e1005n:483": "Nārīrūpaṃ patibbatā;",
  "anya-e1005n:484": "Vijjā rūpaṃ a-rūpānaṃ,",
  "anya-e1005n:485": "Khamā rūpaṃ tapassinaṃ.",
  "anya-e1005n:486": "95.",
  "anya-e1005n:487": " Itthīnañca dhanaṃ rūpaṃ,",
  "anya-e1005n:488": "Purisānaṃ vijjā dhanaṃ;",
  "anya-e1005n:489": "Bhikkhūnañca dhanaṃ sīlaṃ,",
  "anya-e1005n:490": "Rājānañca dhanaṃ balaṃ.",
  "anya-e1005n:491": "96.",
  "anya-e1005n:492": " Tapassino kisā sobhā,",
  "anya-e1005n:493": "Thūlā sobhā catuppadā;",
  "anya-e1005n:494": "Purisā vijjavā sobhā,",
  "anya-e1005n:495": "Itthī sobhāsa sāmikā.",
  "anya-e1005n:496": "97.",
  "anya-e1005n:497": " Pañca ratyā sugandhabbā,",
  "anya-e1005n:498": "Satta ratyā dhanuggahā;",
  "anya-e1005n:499": "Eka māsā subhariyā,",
  "anya-e1005n:500": "Aḍḍha māsā sissā malā.",
  "anya-e1005n:501": "98.",
  "anya-e1005n:502": " Hiṃ ramati paṅa,",
  "anya-e1005n:503": "Haṅa ramati poka.",
  "anya-e1005n:504": "Thī ramati pu,",
  "anya-e1005n:505": "Khu ramati dhaṃ.",
  "anya-e1005n:506": "99.",
  "anya-e1005n:507": " Jiṇṇamannaṃ pasaṃseyya,",
  "anya-e1005n:508": "Dārañca gatayobbanaṃ;",
  "anya-e1005n:509": "Raṇā punāgatā sūraṃ,",
  "anya-e1005n:510": "Sassañca gehamāgataṃ.",
  "anya-e1005n:511": "100.",
  "anya-e1005n:512": " Dvatti-patikā nārī ca,",
  "anya-e1005n:513": "Bhikkhu dvatti-vihāriko;",
  "anya-e1005n:514": "Dvatti-pāsa-mutto pakkhī,",
  "anya-e1005n:515": "Kata-māyā bahūtharaṃ.",
  "anya-e1005n:516": "101.",
  "anya-e1005n:517": " Dujjanaṃ pahārādame,",
  "anya-e1005n:518": "Mittaṃ dame a-bhāṇikā;",
  "anya-e1005n:519": "Itthiñca byasanā dame,",
  "anya-e1005n:520": "Rāginaṃ appa bhojanā.",
  "anya-e1005n:521": "102.",
  "anya-e1005n:522": " Na ratti vinā candimā,",
  "anya-e1005n:523": "Vīciṃ vinā ca aṇṇavo;",
  "anya-e1005n:524": "Haṃsaṃ vinā pokkharaṇī,",
  "anya-e1005n:525": "Patiṃ kaññāca sobhate.",
  "anya-e1005n:526": "103.",
  "anya-e1005n:527": " Patinā janito bhogo,",
  "anya-e1005n:528": "Itthiyāva saṃgoppito;",
  "anya-e1005n:529": "Purisova hi padhāno,",
  "anya-e1005n:530": "Itthī suttaṃva sūciyā.",
  "anya-e1005n:531": "104.",
  "anya-e1005n:532": " Sabbānadī vaṅkanadī,",
  "anya-e1005n:533": "Sabbe kaṭṭhamayā vanā;",
  "anya-e1005n:534": "Sabbitthiyo kare pāpaṃ,",
  "anya-e1005n:535": "Labhamāne nivātake.",
  "anya-e1005n:536": "105.",
  "anya-e1005n:537": " Vivādasīliṃ usūyabhāṇiniṃ,",
  "anya-e1005n:538": "Sampassataṇhiṃ bahupākabhuttiniṃ;",
  "anya-e1005n:539": "Aggantabhuttiṃ paragehavāsiniṃ,",
  "anya-e1005n:540": "Nāriṃ caje puttasatampi pūmā.",
  "anya-e1005n:541": "106.",
  "anya-e1005n:542": " Bhuttesu maṇḍesu janīva kantinī,",
  "anya-e1005n:543": "Guyheca ṭhāne bhaginīva hiriṇī;",
  "anya-e1005n:544": "Kammesu pattesu karoti dāsīva,",
  "anya-e1005n:545": "Bhayesu mantī sayanesu rāmaye;",
  "anya-e1005n:546": "Rūpīsu sikkhī kupanesu khantinī,",
  "anya-e1005n:547": "Sā nārī seṭṭhāti vadanti paṇḍitā;",
  "anya-e1005n:548": "Kāyassa bhedāca divebhaveyya sā.",
  "anya-e1005n:549": "107.",
  "anya-e1005n:550": " Sāmā migakkhī tanumajjhagattā,",
  "anya-e1005n:551": "Sūrū sukesī samadantapantī;",
  "anya-e1005n:552": "Gambhīranābhī yuvatī susīlī,",
  "anya-e1005n:553": "Hīne kule jātāpi vivāhyā.",
  "anya-e1005n:554": "108.",
  "anya-e1005n:555": " Saradaṃratu-kālānaṃ,",
  "anya-e1005n:556": "Bhariyānaṃ rūpavatī;",
  "anya-e1005n:557": "Jeṭṭho padhānaṃ puttānaṃ,",
  "anya-e1005n:558": "Disānaṃ uttarādisā.",
  "anya-e1005n:559": "109.",
  "anya-e1005n:560": " Yā icche puriso hotuṃ,",
  "anya-e1005n:561": "Jāti jāti punappunaṃ;",
  "anya-e1005n:562": "Sāmikaṃ apacāyeyya,",
  "anya-e1005n:563": "Indaṃva pāricārikā.",
  "anya-e1005n:564": "110.",
  "anya-e1005n:565": " Yo icche puriso hotuṃ,",
  "anya-e1005n:566": "Jāti jāti punappunaṃ;",
  "anya-e1005n:567": "Paradānaṃ vivajjeyya,",
  "anya-e1005n:568": "Dhotapādova kaddamaṃ.",
  "anya-e1005n:569": "111.",
  "anya-e1005n:570": " Atikkanta vayo poso,",
  "anya-e1005n:571": "Āneti timbaruttaniṃ;",
  "anya-e1005n:572": "Tassā issā asaddhāti,",
  "anya-e1005n:573": "Taṃ parābhavato mukhaṃ.",
  "anya-e1005n:574": "Itthikaṇḍo niṭṭhito.",
  "anya-e1005n:575": "6. Rājakaṇḍo",
  "anya-e1005n:576": "112.",
  "anya-e1005n:577": " Ekayāmaṃ saye rājā,",
  "anya-e1005n:578": "Dviyāmaññeva paṇḍito;",
  "anya-e1005n:579": "Gharāvāso tiyāmaṃva,",
  "anya-e1005n:580": "Catuyāmaṃ tu yācako.",
  "anya-e1005n:581": "113.",
  "anya-e1005n:582": " Dhanavā sutavā rājā,",
  "anya-e1005n:583": "Nadī vejjo cimepañca;",
  "anya-e1005n:584": "Yattha dese na vijjanti,",
  "anya-e1005n:585": "Na tattha divasaṃ vase.",
  "anya-e1005n:586": "114.",
  "anya-e1005n:587": " Yasmiṃ padese na māno,",
  "anya-e1005n:588": "Na pemaṃ naca bandhavā;",
  "anya-e1005n:589": "Naca vijjāgamo koci,",
  "anya-e1005n:590": "Na tattha divasaṃ vase.",
  "anya-e1005n:591": "115.",
  "anya-e1005n:592": " Aputtakaṃ gharaṃ suññaṃ,",
  "anya-e1005n:593": "Raṭṭhaṃ suññaṃ arājakaṃ;",
  "anya-e1005n:594": "A sippassa mukhaṃ suññaṃ,",
  "anya-e1005n:595": "Sabba suññaṃ daliddakā.",
  "anya-e1005n:596": "116.",
  "anya-e1005n:597": " Dhanamiccheyya vāṇijjo,",
  "anya-e1005n:598": "Vijjamicche bhajesutaṃ;",
  "anya-e1005n:599": "Puttamicche taruṇitthiṃ,",
  "anya-e1005n:600": "Rājāmaccaṃ vasaṃ game.",
  "anya-e1005n:601": "117.",
  "anya-e1005n:602": " Naṭṭhoyati asantuṭṭho,",
  "anya-e1005n:603": "Santuṭṭho ca mahīpati;",
  "anya-e1005n:604": "Lajjā ca gaṇikā naṭṭhā,",
  "anya-e1005n:605": "Nillajjā kuladhītikā.",
  "anya-e1005n:606": "118.",
  "anya-e1005n:607": " Pakkhīnaṃ balamākāso,",
  "anya-e1005n:608": "Macchānamudakaṃ balaṃ;",
  "anya-e1005n:609": "Dubbalassa balaṃ rājā,",
  "anya-e1005n:610": "Kumārānaṃ rudaṃ balaṃ.",
  "anya-e1005n:611": "119.",
  "anya-e1005n:612": " Khamā jāgariyuṭṭhānaṃ,",
  "anya-e1005n:613": "Saṃvibhāgo dayikkhaṇā;",
  "anya-e1005n:614": "Nāyakassa guṇā ete,",
  "anya-e1005n:615": "Icchitabbā sataṃ guṇā.",
  "anya-e1005n:616": "120.",
  "anya-e1005n:617": " Sakiṃ vadanti rājāno,",
  "anya-e1005n:618": "Sakiṃ samaṇabrāhmaṇā;",
  "anya-e1005n:619": "Sakiṃ sappurisā loke,",
  "anya-e1005n:620": "Esa dhammo sanantano.",
  "anya-e1005n:621": "121.",
  "anya-e1005n:622": " Alaso gihī kāmabhogī na sādhu,",
  "anya-e1005n:623": "Asaññato pabbajito na sādhu;",
  "anya-e1005n:624": "Rājā anisammakārī na sādhu,",
  "anya-e1005n:625": "Paṇḍito kodhano taṃpi na sādhu.",
  "anya-e1005n:626": "122.",
  "anya-e1005n:627": " Bahavo yattha nettāro,",
  "anya-e1005n:628": "Sabbe paṇḍitamānino;",
  "anya-e1005n:629": "Sabbe mahattamicchanti,",
  "anya-e1005n:630": "Tesaṃ kammaṃ vinassati.",
  "anya-e1005n:631": "123.",
  "anya-e1005n:632": " Āyaṃ khayaṃ sayaṃ jaññā,",
  "anya-e1005n:633": "Rājā sayaṃ katākataṃ;",
  "anya-e1005n:634": "Niggahe niggahetabbaṃ,",
  "anya-e1005n:635": "Paggahe paggahārahaṃ.",
  "anya-e1005n:636": "124.",
  "anya-e1005n:637": " Piṭṭhitokkaṃ niseveyya,",
  "anya-e1005n:638": "Kucchināva hutāsanaṃ;",
  "anya-e1005n:639": "Sāmikaṃ sabbabhāgena,",
  "anya-e1005n:640": "Paralokaṃ amohavā.",
  "anya-e1005n:641": "125.",
  "anya-e1005n:642": " Aggi āpo itthimūḷho,",
  "anya-e1005n:643": "Sappo rāja-kulānica;",
  "anya-e1005n:644": "Apayantena gantabbā,",
  "anya-e1005n:645": "Acceka-pāṇahārakā.",
  "anya-e1005n:646": "126.",
  "anya-e1005n:647": " Paduṭṭha-bhariya-saṃvāso,",
  "anya-e1005n:648": "Paduṭṭha citta dāsako;",
  "anya-e1005n:649": "Sa-sappe ca ghare vāso,",
  "anya-e1005n:650": "Maccu eva na saṃsayo.",
  "anya-e1005n:651": "127.",
  "anya-e1005n:652": " Mūḷha sisso padesena,",
  "anya-e1005n:653": "Kunārī bharaṇena ca;",
  "anya-e1005n:654": "Asatā sampayogena,",
  "anya-e1005n:655": "Paṇḍitoppavasīdati.",
  "anya-e1005n:656": "128.",
  "anya-e1005n:657": " Mātā puttakaraṃ pāpaṃ,",
  "anya-e1005n:658": "Sissapāpaṃ gurukatā;",
  "anya-e1005n:659": "Rājā raṭṭhakaraṃ pāpaṃ,",
  "anya-e1005n:660": "Rājapāpaṃ purohito.",
  "anya-e1005n:661": "129.",
  "anya-e1005n:662": " Akodhena jine kodhaṃ,",
  "anya-e1005n:663": "Asādhuṃ sādhunā jine;",
  "anya-e1005n:664": "Jine macchariṃ dānena,",
  "anya-e1005n:665": "Saccenālīkavādinaṃ;",
  "anya-e1005n:666": "130.",
  "anya-e1005n:667": " Adantaṃ damanaṃ dānaṃ,",
  "anya-e1005n:668": "Dānaṃ sabbattha sādhakaṃ;",
  "anya-e1005n:669": "Dānena piya vācāya,",
  "anya-e1005n:670": "Unnamanti namanti ca;",
  "anya-e1005n:671": "131.",
  "anya-e1005n:672": " Dānaṃ sinehabhesajjaṃ,",
  "anya-e1005n:673": "Maccheraṃ dosanosadhaṃ;",
  "anya-e1005n:674": "Dānaṃ yasassī bhesajjaṃ,",
  "anya-e1005n:675": "Maccheraṃ kapaṇosadhaṃ.",
  "anya-e1005n:676": "132.",
  "anya-e1005n:677": " Bahūnamappasārānaṃ,",
  "anya-e1005n:678": "Sāmaggiyā jayaṃ jaye;",
  "anya-e1005n:679": "Tiṇehi vattate yottaṃ,",
  "anya-e1005n:680": "Tena nāgopi bajjhate.",
  "anya-e1005n:681": "133.",
  "anya-e1005n:682": " Sahāyo asamatthopi,",
  "anya-e1005n:683": "Tejasā kiṃkarissati;",
  "anya-e1005n:684": "Nivāte jalito aggi,",
  "anya-e1005n:685": "Sayame vūpasampati.",
  "anya-e1005n:686": "134.",
  "anya-e1005n:687": " Na raññā samakaṃ bhuñje,",
  "anya-e1005n:688": "Kāmabhogaṃ kudācanaṃ;",
  "anya-e1005n:689": "Ākappaṃ rasa bhuttiṃvā,",
  "anya-e1005n:690": "Mālā gandha vilepanaṃ;",
  "anya-e1005n:691": "Vatthaṃ sabbaalaṅkāraṃ,",
  "anya-e1005n:692": "Na raññā sadisaṃ kare.",
  "anya-e1005n:693": "135.",
  "anya-e1005n:694": " Na me rājā sakhā hoti,",
  "anya-e1005n:695": "Na rājā hoti methuno;",
  "anya-e1005n:696": "Eso sāmiko mayhanti,",
  "anya-e1005n:697": "Citte niṭṭhaṃ suthāpaye.",
  "anya-e1005n:698": "136.",
  "anya-e1005n:699": " Nātidūre bhaje rañño,",
  "anya-e1005n:700": "Nāccāsannopavātake;",
  "anya-e1005n:701": "Ujuke nātininne ca,",
  "anya-e1005n:702": "Na bhaje uccamāsane.",
  "anya-e1005n:703": "Chadose vajje sevako,",
  "anya-e1005n:704": "Tiṭṭhe aggiṃva saṃyato.",
  "anya-e1005n:705": "137.",
  "anya-e1005n:706": " Guṇī sabbaññu tulyopi,",
  "anya-e1005n:707": "Nasobhati anissayo;",
  "anya-e1005n:708": "Anagghamopi maṇiseṭṭho,",
  "anya-e1005n:709": "Hemaṃ nissāya sobhati.",
  "anya-e1005n:710": "Rājakaṇḍo niṭṭhito.",
  "anya-e1005n:711": "7. Pakiṇṇakakaṇḍo",
  "anya-e1005n:712": "138.",
  "anya-e1005n:713": " Itthimisse kutosīlaṃ,",
  "anya-e1005n:714": "Maṃsa bhakkhe kutodayā;",
  "anya-e1005n:715": "Surā pāne kutosaccaṃ,",
  "anya-e1005n:716": "Mahālobhe kutohirī;",
  "anya-e1005n:717": "Mahātande kutosippaṃ,",
  "anya-e1005n:718": "Mahā kodhe kutodhanaṃ.",
  "anya-e1005n:719": "139.",
  "anya-e1005n:720": " Surā yogo vikālo ca,",
  "anya-e1005n:721": "Samajja caraṇālasaṃ;",
  "anya-e1005n:722": "Khiḍḍādhutto pāpamitto,",
  "anya-e1005n:723": "Bhoganāsamukhā ime.",
  "anya-e1005n:724": "140.",
  "anya-e1005n:725": " Divā nādikkhā vattabbaṃ,",
  "anya-e1005n:726": "Ratto nāvacanena ca;",
  "anya-e1005n:727": "Sañcareyya bhayā bhīto,",
  "anya-e1005n:728": "Vane vanacarī yathā.",
  "anya-e1005n:729": "141.",
  "anya-e1005n:730": " Jīvantāpi matāpañca,",
  "anya-e1005n:731": "Byāsena parikittitā;",
  "anya-e1005n:732": "Dukkhito byādhitomūḷho,",
  "anya-e1005n:733": "Iṇavā nityasevako.",
  "anya-e1005n:734": "142.",
  "anya-e1005n:735": " Anāgataṃ bhayaṃ disvā,",
  "anya-e1005n:736": "Dūrato parivajjaye;",
  "anya-e1005n:737": "Āgatañca bhayaṃ disvā,",
  "anya-e1005n:738": "A bhīto hoti paṇḍito.",
  "anya-e1005n:739": "143.",
  "anya-e1005n:740": " Niddāluko pamattoca,",
  "anya-e1005n:741": "Sukhatto rogavālaso;",
  "anya-e1005n:742": "Mahiccho kammārāmoca,",
  "anya-e1005n:743": "Satte te satthavajjitā.",
  "anya-e1005n:744": "144.",
  "anya-e1005n:745": " Duggataṃ gaccha he lābha,",
  "anya-e1005n:746": "Lābhī lābhena pūrati;",
  "anya-e1005n:747": "Thale pavassa pajjunna,",
  "anya-e1005n:748": "Sindhu āpena pūrati;",
  "anya-e1005n:749": "Natthidaṃ kammappadhānakaṃ.",
  "anya-e1005n:750": "145.",
  "anya-e1005n:751": " Na hi koci kate kicce,",
  "anya-e1005n:752": "Kattāraṃ samupekkhate;",
  "anya-e1005n:753": "Tasmā sabbāni kiccāni,",
  "anya-e1005n:754": "Sāva sesena kāraye.",
  "anya-e1005n:755": "146.",
  "anya-e1005n:756": " Tūlaṃ sallahukaṃ loke,",
  "anya-e1005n:757": "Tato cāpalla-jātiko;",
  "anya-e1005n:758": "Tato vuḍḍha manovādo,",
  "anya-e1005n:759": "Pamatto buddhasāsane.",
  "anya-e1005n:760": "147.",
  "anya-e1005n:761": " Pāsāṇachattaṃ garukaṃ,",
  "anya-e1005n:762": "Tato devānacikkhaṇaṃ;",
  "anya-e1005n:763": "Tato vuḍḍhānamovādo,",
  "anya-e1005n:764": "Tato buddhassa sāsanaṃ.",
  "anya-e1005n:765": "148.",
  "anya-e1005n:766": " Kāyassa dakkhiṇa hattho,",
  "anya-e1005n:767": "Doso ettha kaniṭṭhako;",
  "anya-e1005n:768": "Kaṇṇa ghānāna-makkhīnaṃ,",
  "anya-e1005n:769": "Vāmo tu pāda-pāsako.",
  "anya-e1005n:770": "149.",
  "anya-e1005n:771": " Tambūlassa majjha patte,",
  "anya-e1005n:772": "Kuvero rakkhatī sadā;",
  "anya-e1005n:773": "Mūlamhi rakkhati yakkho,",
  "anya-e1005n:774": "Aggamhi kālakaṇṇikā;",
  "anya-e1005n:775": "Tāni bhuñjeyya chinditvā,",
  "anya-e1005n:776": "Sirī evaṃ pavaḍḍhati.",
  "anya-e1005n:777": "150.",
  "anya-e1005n:778": " Sampuṇṇarakkho brahmāva,",
  "anya-e1005n:779": "Accurakkho ca bissaṇo;",
  "anya-e1005n:780": "Tasmā hi te pūjayantu,",
  "anya-e1005n:781": "Sadā mānenti taṃ naraṃ.",
  "anya-e1005n:782": "151.",
  "anya-e1005n:783": " Goṇā hi sabbagihīnaṃ,",
  "anya-e1005n:784": "Posakā bhogadāyakā;",
  "anya-e1005n:785": "Tasmā hi mātā pitūva,",
  "anya-e1005n:786": "Mānaye sakkareyya ca.",
  "anya-e1005n:787": "152.",
  "anya-e1005n:788": " Yeca khādanti gomaṃsaṃ,",
  "anya-e1005n:789": "Mātu maṃsaṃva khādare;",
  "anya-e1005n:790": "Matesu tesu gijjhānaṃ,",
  "anya-e1005n:791": "Dade sote ca vāhaye.",
  "anya-e1005n:792": "153.",
  "anya-e1005n:793": " Gurusiddho sippārambho,",
  "anya-e1005n:794": "Ravi sokrā ca majjhimo;",
  "anya-e1005n:795": "Na sippo buddhacandaro,",
  "anya-e1005n:796": "Sorī aṅgāca maraṇaṃ.",
  "anya-e1005n:797": "154.",
  "anya-e1005n:798": " Aṭṭhamiyaṃ guruṃ hanti,",
  "anya-e1005n:799": "Sissaṃ hanti catuddasiṃ;",
  "anya-e1005n:800": "Sippaṃ hanti dasa sippaṃ,",
  "anya-e1005n:801": "Mātāpitā ca puṇṇamiṃ.",
  "anya-e1005n:802": "155.",
  "anya-e1005n:803": " Nāḷikaṃ satta nabhuñje,",
  "anya-e1005n:804": "Na lābuṃ navamaṃ tathā;",
  "anya-e1005n:805": "Dvādasa prinnaṃtriminaṃ,",
  "anya-e1005n:806": "Bhuñje sippaṃ vinassati.",
  "anya-e1005n:807": "156.",
  "anya-e1005n:808": " Ekaṃ caje kulaatthaṃ,",
  "anya-e1005n:809": "Gāmassatthaṃ kuvaṃ caje;",
  "anya-e1005n:810": "Gāma caje janapadatthaṃ,",
  "anya-e1005n:811": "Attatthaṃ pathaviṃ caje.",
  "anya-e1005n:812": "157.",
  "anya-e1005n:813": " Desaṃ ossajja gacchanti,",
  "anya-e1005n:814": "Sīho sappuriso gajo;",
  "anya-e1005n:815": "Tattheva nidhanaṃ yanti,",
  "anya-e1005n:816": "Kāko kāpuriso migo.",
  "anya-e1005n:817": "158.",
  "anya-e1005n:818": " Yamhi padese na māno,",
  "anya-e1005n:819": "Na pemaṃ na ca bandhavā;",
  "anya-e1005n:820": "Na ca vijjāgāho koci,",
  "anya-e1005n:821": "Na tattha vasanaṃ kare.",
  "anya-e1005n:822": "159.",
  "anya-e1005n:823": " Caratyekena pādena,",
  "anya-e1005n:824": "Tiṭṭhatyekena paṇḍito;",
  "anya-e1005n:825": "A nisamma paraṃ ṭhānaṃ,",
  "anya-e1005n:826": "Na pubbamālayaṃ jahe.",
  "anya-e1005n:827": "160.",
  "anya-e1005n:828": " Dhana dhañña payogesu,",
  "anya-e1005n:829": "Tathā vijjāgamesu ca;",
  "anya-e1005n:830": "Dūtesu apacāresu,",
  "anya-e1005n:831": "Cajjā lajjā tadā bhave.",
  "anya-e1005n:832": "161.",
  "anya-e1005n:833": " Dvi guṇo thīnamāhāro,",
  "anya-e1005n:834": "Buddhicāpi catugguṇo;",
  "anya-e1005n:835": "Chagguṇo hoti vāyāmo,",
  "anya-e1005n:836": "Kāmotvaṭṭha-guṇo bhave.",
  "anya-e1005n:837": "162.",
  "anya-e1005n:838": " Pabbe pabbe kamenucchu,",
  "anya-e1005n:839": "Visesarasavāggato;",
  "anya-e1005n:840": "Tathā sumettiko sādhu,",
  "anya-e1005n:841": "Viparītova dujjano.",
  "anya-e1005n:842": "163.",
  "anya-e1005n:843": " Kassako vāṇijo macco,",
  "anya-e1005n:844": "Samaṇo suta sīlavā;",
  "anya-e1005n:845": "Tesu vipula jātesu,",
  "anya-e1005n:846": "Raṭṭhampi vipulaṃ siyā.",
  "anya-e1005n:847": "164.",
  "anya-e1005n:848": " Asajjhāya malā mantā,",
  "anya-e1005n:849": "Anuṭṭhāna malā gharā;",
  "anya-e1005n:850": "Malaṃ vaṇṇassa kosajjaṃ,",
  "anya-e1005n:851": "Pamādo rakkhato malaṃ.",
  "anya-e1005n:852": "165.",
  "anya-e1005n:853": " Hīnānaṃ gacchate vittaṃ,",
  "anya-e1005n:854": "Vīrānaṃ santakattanaṃ;",
  "anya-e1005n:855": "Vadanti ca hīnā janā,",
  "anya-e1005n:856": "Pubba-kammappadhānakā.",
  "anya-e1005n:857": "166.",
  "anya-e1005n:858": " Na vadanti cevaṃdhīrā,",
  "anya-e1005n:859": "Vāyamiṃsu sabbakamme;",
  "anya-e1005n:860": "Na ce sijjhati taṃ kammaṃ,",
  "anya-e1005n:861": "A-phalaṃ eva ko doso.",
  "anya-e1005n:862": "167.",
  "anya-e1005n:863": " Nīcaṃ kulaṃ nipaññaṃ vā,",
  "anya-e1005n:864": "Nirūpaṃ nibalaṃ samaṃ;",
  "anya-e1005n:865": "Imaṃ kālaṃ chuttakālaṃ,",
  "anya-e1005n:866": "Dhanameva visesakaṃ.",
  "anya-e1005n:867": "Pakiṇṇakakaṇḍo niṭṭhito.",
  "anya-e1005n:868": "Paṇḍito sujano kaṇḍo,",
  "anya-e1005n:869": "Dujjano mitta-itthi ca;",
  "anya-e1005n:870": "Rājā pakiṇṇako cāti,",
  "anya-e1005n:871": "Satta-kaṇḍa-vibhūsitaṃ.",
  "anya-e1005n:872": "Visuddhā cāra-therena,",
  "anya-e1005n:873": "Visuddhārāma-vāsinā;",
  "anya-e1005n:874": "Sabba-kulānamatthāya,",
  "anya-e1005n:875": "Visodhitaṃ pathakkhaye."
}