{
  "anya-e1213n:0": "Saddabindu pakaraṇaṃ",
  "anya-e1213n:1": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1213n:2": "1.",
  "anya-e1213n:3": " Yassañeyyesu dhammesu, nāṇumattampaveditaṃ,",
  "anya-e1213n:4": "Natvāsaddhammasaṅghaṃtaṃ, saddabinduṃsamārabhe;",
  "anya-e1213n:5": "2. ",
  "anya-e1213n:6": " Kādiritā navasaṅkhyā, kamenaṭā di yādica,",
  "anya-e1213n:7": "Pādayopañca saṅkhyātā, suññanāmā saraññanā;",
  "anya-e1213n:8": "3.",
  "anya-e1213n:9": " Sarehevasarāpubbe luttāvāvīpareramā,",
  "anya-e1213n:10": "Byañjanācāgamāvāvī dīgharassādisambhavā;",
  "anya-e1213n:11": "4. ",
  "anya-e1213n:12": " Kākāsenāgatosisa keniddhimaccadassayi,",
  "anya-e1213n:13": "Arājakhvaggimesīnaṃ sotukammeghayitthiyo;",
  "anya-e1213n:14": "Iti sandhikappo samatto.",
  "anya-e1213n:15": "5. ",
  "anya-e1213n:16": " Buddhapumayuvasanta rājabrahmasakhācasā,",
  "anya-e1213n:17": "Yatādidehījantuca satthupitābhibhūvidū;",
  "anya-e1213n:18": "6.",
  "anya-e1213n:19": " Kaññāmmārattithipo, kkharaṇīnadirumātubhū,",
  "anya-e1213n:20": "Napuṃsaketiyantāca, padakammadadhāyuno;",
  "anya-e1213n:21": "7. ",
  "anya-e1213n:22": " Gahitāgahaṇenettha suddhosyādyantakāpume,",
  "anya-e1213n:23": "Vimalāhontijāntehi thyaṃpañcantehidādhikā;",
  "anya-e1213n:24": "8.",
  "anya-e1213n:25": " Napuṃsakepayogātu janakāhontityantato,",
  "anya-e1213n:26": "Padhānānugatāsabba nāmasamāsataddhitā;",
  "anya-e1213n:27": "9. ",
  "anya-e1213n:28": " Attiliṅgānipātādi tatoluttāvasyādayo,",
  "anya-e1213n:29": "Suttānurūpatosiddhā hontivattāmanādayo;",
  "anya-e1213n:30": "Iti nāmakappo samatto.",
  "anya-e1213n:31": "10.",
  "anya-e1213n:32": " Chakārakesasāmismiṃ samāsohotisambhavā,",
  "anya-e1213n:33": "Taddhītākattukammasa, mpadānokāsasāmisu;",
  "anya-e1213n:34": "11. ",
  "anya-e1213n:35": " Sādhattayamhiākhyāto kitakosattasādhane,",
  "anya-e1213n:36": "Sabbatthapaṭhamāvutte avuttedutiyādayo;",
  "anya-e1213n:37": "12.",
  "anya-e1213n:38": " Manasāmuninovutyā vanebuddhenavaṇṇite,",
  "anya-e1213n:39": "Vaṭṭāhitovivaṭṭatthaṃ bhikkhubhāvetibhāvanaṃ;",
  "anya-e1213n:40": "Iti kārakakappo samatto.",
  "anya-e1213n:41": "13. ",
  "anya-e1213n:42": " Rāsīdvipadikādvandā liṅgenavacanenaca,",
  "anya-e1213n:43": "Luttātulyādhikaraṇā bahubbīhītukhemarū;",
  "anya-e1213n:44": "14.",
  "anya-e1213n:45": " Tappurisācakhemorā dayācakammadhārayā,",
  "anya-e1213n:46": "Digavocāvyayāhārā etesabbepihāritā;",
  "anya-e1213n:47": "Iti samāsakappo samatto.",
  "anya-e1213n:48": "15.",
  "anya-e1213n:49": " Kaccāditopiekamhā saddatoniyamaṃvinā,",
  "anya-e1213n:50": "Nekatthesatibhonteva sabbetaddhitapaccayā;",
  "anya-e1213n:51": "Iti taddhitakappo samatto.",
  "anya-e1213n:52": "16. ",
  "anya-e1213n:53": " Kattarināññathākamme tathābhāvetumerayā,",
  "anya-e1213n:54": "Sabbetepacadhātumhi saṅkhepenamarūmayā;",
  "anya-e1213n:55": "17.",
  "anya-e1213n:56": " Gamīmhātiguṇāphatto sambhavāaññadhātusu,",
  "anya-e1213n:57": "Anantāvapayogāte ādesapaccayādihi;",
  "anya-e1213n:58": "Iti ākhyātakappo samatto.",
  "anya-e1213n:59": "18.",
  "anya-e1213n:60": " Kitādipaccayāsabbe, ekamhāapidhātuto,",
  "anya-e1213n:61": "Siyuṃnurūpatosatta, sādhanesatipāyato;",
  "anya-e1213n:62": "Iti kitakappo samatto.",
  "anya-e1213n:63": "19. ",
  "anya-e1213n:64": " Iminākiñcilesena, sakkāñātuṃjināgame,",
  "anya-e1213n:65": "Payogāñāṇināsindhu, rasovekenabindunā;",
  "anya-e1213n:66": "20.",
  "anya-e1213n:67": " Rammaṃsīghappavesāya, puraṃpiṭakasaññitaṃ,",
  "anya-e1213n:68": "Maggojumaggataṃmaggaṃ, saddāraññevisodhito;",
  "anya-e1213n:69": "21. ",
  "anya-e1213n:70": " Teneva kiñci jalito jalito padīpo",
  "anya-e1213n:71": "Kaccāyanuttiratano citagabbhakoṇe,",
  "anya-e1213n:72": "Dhammādirājagurunā garumāmakena",
  "anya-e1213n:73": "Dhammena yobbipatinā sagaruttanīto;",
  "anya-e1213n:74": "Iti saddabindu pakaraṇaṃ parisamattaṃ.",
  "anya-e1213n:75": "Yosaññamo guṇadhano nayanaṃ nijaṃva",
  "anya-e1213n:76": "Sikkhāpayī mama mavaṃ sugatāgamādo,",
  "anya-e1213n:77": "Salloka puñja suhado padumādi rāma",
  "anya-e1213n:78": "Nāmo mahā yativarā cariyo samayhaṃ;",
  "anya-e1213n:79": "Saddhādhanena vasatā viditamhi pupphā",
  "anya-e1213n:80": "Rāmedhunā ariyavaṃsa dhajavhayena,",
  "anya-e1213n:81": "Santena ñāṇatilako tyaparākhyakena",
  "anya-e1213n:82": "Bālānametamavidhīyi mayāhitāya;"
}