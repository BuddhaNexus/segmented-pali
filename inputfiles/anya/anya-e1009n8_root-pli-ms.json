{
  "anya-e1009n8:0": "Gharāvāsa-niddesa",
  "anya-e1009n8:1": "171.",
  "anya-e1009n8:2": " Dukkhaṃ gahabbataṃ sādhu,",
  "anya-e1009n8:3": "Saṃvibhajjañca bhojanaṃ;",
  "anya-e1009n8:4": "A-hāso attha-lobhesu,",
  "anya-e1009n8:5": "Attha-byāpatti abyatho.",
  "anya-e1009n8:6": "172.",
  "anya-e1009n8:7": " Yodha sītañca uṇhañca,",
  "anya-e1009n8:8": "Tiṇā bhiyyo na maññati;",
  "anya-e1009n8:9": "Karaṃ purisa-kiccāni,",
  "anya-e1009n8:10": "So sukhā na vihāyati.",
  "anya-e1009n8:11": "173.",
  "anya-e1009n8:12": " Paṇḍito sīla-sampanno,",
  "anya-e1009n8:13": "Saṇhā ca paṭibhānavā;",
  "anya-e1009n8:14": "Nivāta-vutti atthaddho,",
  "anya-e1009n8:15": "Tādiso labhate yasaṃ.",
  "anya-e1009n8:16": "174.",
  "anya-e1009n8:17": " Uṭṭhānako analaso,",
  "anya-e1009n8:18": "Āpadāsu na vedhati;",
  "anya-e1009n8:19": "Acchinnavutti medhāvī,",
  "anya-e1009n8:20": "Tādiso labhate yasaṃ.",
  "anya-e1009n8:21": "175.",
  "anya-e1009n8:22": " Saṅgāhako mitta-karo,",
  "anya-e1009n8:23": "Vadaññū vītta-maccharo;",
  "anya-e1009n8:24": "Netā vi-netā anu-netā,",
  "anya-e1009n8:25": "Tādiso labhate yasaṃ.",
  "anya-e1009n8:26": "176.",
  "anya-e1009n8:27": " Uṭṭhānavako satīmato,",
  "anya-e1009n8:28": "Suci-kammassa nisammakārino;",
  "anya-e1009n8:29": "Saññatassa dhamma-jīvino,",
  "anya-e1009n8:30": "A-ppamattassa yasobhi-vaḍḍhati.",
  "anya-e1009n8:31": "177.",
  "anya-e1009n8:32": " Dveva tāta padākāni,",
  "anya-e1009n8:33": "Yattha sabbaṃ patiṭṭhitaṃ;",
  "anya-e1009n8:34": "A-laddhassa ca yo lābho,",
  "anya-e1009n8:35": "Laddhassa anurakkhaṇā.",
  "anya-e1009n8:36": "178.",
  "anya-e1009n8:37": " Catudhā vibhaje bhoge,",
  "anya-e1009n8:38": "Paṇḍito gharamāvasaṃ;",
  "anya-e1009n8:39": "Ekena bhogaṃ bhuñjeyya,",
  "anya-e1009n8:40": "Dvīhi kammaṃ payojaye;",
  "anya-e1009n8:41": "Catutthañca nidhāpeyya,",
  "anya-e1009n8:42": "Āpadāsu bhavissati.",
  "anya-e1009n8:43": "179.",
  "anya-e1009n8:44": " Añjanānaṃ khayaṃ disvā,",
  "anya-e1009n8:45": "Upacikānañca ācayaṃ;",
  "anya-e1009n8:46": "Madhūnañca samāhāraṃ,",
  "anya-e1009n8:47": "Paṇḍitto gharamāvase.",
  "anya-e1009n8:48": "180.",
  "anya-e1009n8:49": " Vibhavaṃ rakkhato laddhaṃ,",
  "anya-e1009n8:50": "Parihāni na vijjati;",
  "anya-e1009n8:51": "Ārakkhamhi a-santamhi,",
  "anya-e1009n8:52": "Laddhaṃ laddhaṃ vinassati.",
  "anya-e1009n8:53": "181.",
  "anya-e1009n8:54": " Paññā natthi dhanaṃ natthi,",
  "anya-e1009n8:55": "Yassa loke na vindati;",
  "anya-e1009n8:56": "Putta-dārā na pīyanti,",
  "anya-e1009n8:57": "Tassa mittaṃ sukhāvahaṃ.",
  "anya-e1009n8:58": "182.",
  "anya-e1009n8:59": " Cattāro ca veditabbā,",
  "anya-e1009n8:60": "Mittā ceva suhadā ca;",
  "anya-e1009n8:61": "Upakāro suhadopi,",
  "anya-e1009n8:62": "Samāna-sukha-dukkho ca;",
  "anya-e1009n8:63": "Atthakkhāyīnukampako,",
  "anya-e1009n8:64": "Tathā mitto veditabbo.",
  "anya-e1009n8:65": "183.",
  "anya-e1009n8:66": " Bhogā naṭṭhena jiṇṇena,",
  "anya-e1009n8:67": "A-mitena ca bhojane;",
  "anya-e1009n8:68": "Na tiṭṭhanti ciraṃ disvā,",
  "anya-e1009n8:69": "Taṃ paṇḍito ghare vase.",
  "anya-e1009n8:70": "184.",
  "anya-e1009n8:71": " Ati-sītaṃ ati-uṇhaṃ,",
  "anya-e1009n8:72": "Ati-sāyamidaṃ ahu;",
  "anya-e1009n8:73": "Iti vissaṭṭha-kammantte,",
  "anya-e1009n8:74": "Atthā accenti māṇave.",
  "anya-e1009n8:75": "185.",
  "anya-e1009n8:76": " Na divā suppa-sīlena,",
  "anya-e1009n8:77": "Rattinaṭṭhānadessinā;",
  "anya-e1009n8:78": "Niccaṃ mattena soṇḍena,",
  "anya-e1009n8:79": "Sakkā āvasituṃ gharaṃ.",
  "anya-e1009n8:80": "186.",
  "anya-e1009n8:81": " Hananti bhogā dummedhaṃ,",
  "anya-e1009n8:82": "No ce pāra-gavesino;",
  "anya-e1009n8:83": "Bhoga-taṇhāya dummedho,",
  "anya-e1009n8:84": "Hanti aññeva attanaṃ.",
  "anya-e1009n8:85": "187.",
  "anya-e1009n8:86": " Dujjīvitamajīvimhā ,",
  "anya-e1009n8:87": "Yesaṃ no na dadāmase;",
  "anya-e1009n8:88": "Vijjamānesu bhogesu,",
  "anya-e1009n8:89": "Dīpaṃ nā kamha mattano.",
  "anya-e1009n8:90": "188.",
  "anya-e1009n8:91": " Saṭṭhi-vassa-sahassāni ,",
  "anya-e1009n8:92": "Paripuṇṇāni sabbaso;",
  "anya-e1009n8:93": "Niraye paccamānānaṃ,",
  "anya-e1009n8:94": "Kadā anto bhavissati.",
  "anya-e1009n8:95": "189.",
  "anya-e1009n8:96": " Natthi anto kuto anto,",
  "anya-e1009n8:97": "Na anto patidissati;",
  "anya-e1009n8:98": "Tadā hi pakataṃ pāpaṃ,",
  "anya-e1009n8:99": "Mama tuyhañce mārisā;",
  "anya-e1009n8:100": "190.",
  "anya-e1009n8:101": " Sohaṃ nūna ito gantā,",
  "anya-e1009n8:102": "Yoni laddhāna mānusaṃ;",
  "anya-e1009n8:103": "Vadaññū sīla-sampanno,",
  "anya-e1009n8:104": "Kāhāmi kusalaṃ bahuṃ.",
  "anya-e1009n8:105": "191.",
  "anya-e1009n8:106": " Mā gijjhe paccaye macco,",
  "anya-e1009n8:107": "Bahu-dosā hi paccayā;",
  "anya-e1009n8:108": "Caranto paccaye ñāyā,",
  "anya-e1009n8:109": "Ubhayatthāpi vaḍḍhati.",
  "anya-e1009n8:110": "192.",
  "anya-e1009n8:111": " A-laddhā vittaṃ tappati,",
  "anya-e1009n8:112": "Pubbe a-samudānitaṃ;",
  "anya-e1009n8:113": "Na pubbe dhanamesissaṃ,",
  "anya-e1009n8:114": "Iti pacchānutappati.",
  "anya-e1009n8:115": "193.",
  "anya-e1009n8:116": " Kūṭavedī pure āsiṃ,",
  "anya-e1009n8:117": "Pisuṇo piṭṭhi-maṃsiko;",
  "anya-e1009n8:118": "Caṇḍo ca pharuso cāpi,",
  "anya-e1009n8:119": "Iti pacchānutappati.",
  "anya-e1009n8:120": "194.",
  "anya-e1009n8:121": " Pāṇātipātī pure āsiṃ,",
  "anya-e1009n8:122": "Luddo cāpi anariyo;",
  "anya-e1009n8:123": "Bhūtānaṃ nānukampiyaṃ,",
  "anya-e1009n8:124": "Iti pacchānutappati.",
  "anya-e1009n8:125": "195.",
  "anya-e1009n8:126": " Bahūsu vata santīsu,",
  "anya-e1009n8:127": "Anāpādāsu itthisu;",
  "anya-e1009n8:128": "Para-dāraṃ asevissaṃ,",
  "anya-e1009n8:129": "Iti pacchānutappati.",
  "anya-e1009n8:130": "196.",
  "anya-e1009n8:131": " Bahumhi tava santamhi,",
  "anya-e1009n8:132": "Anna-pāne upaṭṭhite;",
  "anya-e1009n8:133": "Na pubbe adadaṃ dānaṃ,",
  "anya-e1009n8:134": "Iti pacchānutappati.",
  "anya-e1009n8:135": "197.",
  "anya-e1009n8:136": " Mātaraṃ pitarañcāpi,",
  "anya-e1009n8:137": "Jiṇṇakaṃ gata-yobbanaṃ;",
  "anya-e1009n8:138": "Pahu santo na posissaṃ,",
  "anya-e1009n8:139": "Iti pacchānukappati.",
  "anya-e1009n8:140": "198.",
  "anya-e1009n8:141": " Ācariyamanusatthāraṃ ,",
  "anya-e1009n8:142": "Sabba-kāma-rasāharaṃ;",
  "anya-e1009n8:143": "Pitaraṃ atimaññissaṃ,",
  "anya-e1009n8:144": "Iti pacchānutappati.",
  "anya-e1009n8:145": "199.",
  "anya-e1009n8:146": " Samaṇe brāhmaṇe cāpi,",
  "anya-e1009n8:147": "Sīlavante bahussute;",
  "anya-e1009n8:148": "Na pubbe payirupāsissaṃ,",
  "anya-e1009n8:149": "Iti pacchānutappati.",
  "anya-e1009n8:150": "200.",
  "anya-e1009n8:151": " Sādhu hoti tapo ciṇṇo,",
  "anya-e1009n8:152": "Santo ca payirupāsito;",
  "anya-e1009n8:153": "Na pubbeva tapociṇṇo,",
  "anya-e1009n8:154": "Iti pacchānutappati.",
  "anya-e1009n8:155": "201.",
  "anya-e1009n8:156": " Yo ca etāni ṭhānāni,",
  "anya-e1009n8:157": "Yoniso paṭipajjati;",
  "anya-e1009n8:158": "Karaṃ purisa-kiccāni,",
  "anya-e1009n8:159": "Sa pacchā nānutappati.",
  "anya-e1009n8:160": "202.",
  "anya-e1009n8:161": " Na sādhāraṇa-dārassa,",
  "anya-e1009n8:162": "Na bhuñje sādhumekako;",
  "anya-e1009n8:163": "Na seve lokāyatikaṃ,",
  "anya-e1009n8:164": "Netaṃ paññāya vaḍḍhanaṃ.",
  "anya-e1009n8:165": "203.",
  "anya-e1009n8:166": " Sīlavā vatta-sampanno,",
  "anya-e1009n8:167": "A-ppamatto vicakkhaṇo;",
  "anya-e1009n8:168": "Nivātta-vutti atthaddho,",
  "anya-e1009n8:169": "Surato sakhito mudu.",
  "anya-e1009n8:170": "204.",
  "anya-e1009n8:171": " Saṅgahetā ca mittānaṃ,",
  "anya-e1009n8:172": "Saṃvibhāgī vīdhānavā;",
  "anya-e1009n8:173": "Tappeyya anna-pānena,",
  "anya-e1009n8:174": "Sadā samaṇa-brāhmaṇe.",
  "anya-e1009n8:175": "205.",
  "anya-e1009n8:176": " Dhamma-kāmo sutā-dhāro,",
  "anya-e1009n8:177": "Bhaveyya paripucchako;",
  "anya-e1009n8:178": "Sakkaccaṃ payirupāseyya,",
  "anya-e1009n8:179": "Sīlavante bahussutte.",
  "anya-e1009n8:180": "206.",
  "anya-e1009n8:181": " Gharamāvasamānassa ,",
  "anya-e1009n8:182": "Gahaṭṭhassa sakaṃ gharaṃ;",
  "anya-e1009n8:183": "Khemā vutti siyā evaṃ,",
  "anya-e1009n8:184": "Evaṃ nu assa saṅgaho.",
  "anya-e1009n8:185": "207.",
  "anya-e1009n8:186": " A-byapajjo siyā evaṃ,",
  "anya-e1009n8:187": "Sacca-vādī ca māṇavo;",
  "anya-e1009n8:188": "Asmā lokā paraṃ lokaṃ,",
  "anya-e1009n8:189": "Evaṃ pecca na socati.",
  "anya-e1009n8:190": "208.",
  "anya-e1009n8:191": " Sukhā matteyyatā loke,",
  "anya-e1009n8:192": "Atho petteyyatā sukhā;",
  "anya-e1009n8:193": "Sukhā sāmaññatā loke,",
  "anya-e1009n8:194": "Atho brahmaññatā sukhā.",
  "anya-e1009n8:195": "209.",
  "anya-e1009n8:196": " Pathavī veḷukaṃ pattaṃ,",
  "anya-e1009n8:197": "Cakkavāḷaṃ sucipphalaṃ;",
  "anya-e1009n8:198": "Sineru vammiko khuddo,",
  "anya-e1009n8:199": "Samuddo pātiko yathā.",
  "anya-e1009n8:200": "210.",
  "anya-e1009n8:201": " Brahmāti mātā-pitaro,",
  "anya-e1009n8:202": "Pubbācariyāti vuccare;",
  "anya-e1009n8:203": "Āhuneyyā ca puttānaṃ,",
  "anya-e1009n8:204": "Pajānamanukampakā.",
  "anya-e1009n8:205": "211.",
  "anya-e1009n8:206": " Tasmā hi ne namaseyya,",
  "anya-e1009n8:207": "Sakkareyya ca paṇḍito;",
  "anya-e1009n8:208": "Annena atho pānena,",
  "anya-e1009n8:209": "Vatthena sayanena ca."
}