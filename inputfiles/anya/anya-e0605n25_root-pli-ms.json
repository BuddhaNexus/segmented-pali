{
  "anya-e0605n25:0": "Pāṭihāriyadīpanīgāthā",
  "anya-e0605n25:1": "175.",
  "anya-e0605n25:2": " Brahmassa saddaṃ karavīkabhāṇiṃ,",
  "anya-e0605n25:3": "Yathicchitaṃ sāvayituṃ samatthaṃ;",
  "anya-e0605n25:4": "Saccaṃ piyaṃ bhūtahitaṃ vadantaṃ,",
  "anya-e0605n25:5": "Na pūjaye ko hi naro sacetano.",
  "anya-e0605n25:6": "176.",
  "anya-e0605n25:7": " Iddhi ca ādesanānusāsanī,",
  "anya-e0605n25:8": "Pāṭihīre bhagavā vasī ahu;",
  "anya-e0605n25:9": "Katvāna accherasupāṭihīraṃ,",
  "anya-e0605n25:10": "Desesi dhammaṃ anukampimaṃ pajaṃ."
}