{
  "anya-e1006n0:0": " Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1006n0:1": "Suttantanīti",
  "anya-e1006n0:2": "1.",
  "anya-e1006n0:3": " Parā bhavantaṁ purisaṁ,",
  "anya-e1006n0:4": "Mayaṁ pucchāma gotamaṁ;",
  "anya-e1006n0:5": "Bhavantaṁ puṭṭhu māgamma,",
  "anya-e1006n0:6": "Kiṁ parābhavato mukhaṁ.",
  "anya-e1006n0:7": "2.",
  "anya-e1006n0:8": " Suvijāno bhavaṁhoti,",
  "anya-e1006n0:9": "Duvijāno parābhavo;",
  "anya-e1006n0:10": "Dhammakāmo bhavaṁ hoti,",
  "anya-e1006n0:11": "Dhammadessī parābhavo.",
  "anya-e1006n0:12": "3.",
  "anya-e1006n0:13": " Itihe taṁ vijānāma,",
  "anya-e1006n0:14": "Pathamo so parābhavo;",
  "anya-e1006n0:15": "Dutīyaṁ bhagavā brūhi,",
  "anya-e1006n0:16": "Kiṁ parābhavato mukhaṁ.",
  "anya-e1006n0:17": "4.",
  "anya-e1006n0:18": " Asantassa piyo hoti,",
  "anya-e1006n0:19": "Sante na kurute piyaṁ;",
  "anya-e1006n0:20": "Asataṁ dhammaṁ roceti,",
  "anya-e1006n0:21": "Taṁ parābhavato mukhaṁ.",
  "anya-e1006n0:22": "Kammāparādhasattānaṁ ,",
  "anya-e1006n0:23": "Vināse paccupaṭṭhite;",
  "anya-e1006n0:24": "Anayo nayarūpena,",
  "anya-e1006n0:25": "Buddhimākamya tiṭṭhati.",
  "anya-e1006n0:26": "5.",
  "anya-e1006n0:27": " Niddāsīlī sabhāsīlī,",
  "anya-e1006n0:28": "Anuṭṭhātā ca yo naro;",
  "anya-e1006n0:29": "Alaso kodhapaññāṇo,",
  "anya-e1006n0:30": "Taṁ parābhavato mukhaṁ.",
  "anya-e1006n0:31": "6.",
  "anya-e1006n0:32": " Yo mātaraṁ pitaraṁ vā,",
  "anya-e1006n0:33": "Jiṇṇakaṁ gatayobbanaṁ;",
  "anya-e1006n0:34": "Pahusanto na bharati,",
  "anya-e1006n0:35": "Taṁ parābhavato mukhaṁ.",
  "anya-e1006n0:36": "7.",
  "anya-e1006n0:37": " Yo brāhmaṇaṁ samaṇaṁ vā,",
  "anya-e1006n0:38": "Aññaṁ vāpi vaṇibbakaṁ;",
  "anya-e1006n0:39": "Musāvādena vañceti,",
  "anya-e1006n0:40": "Taṁ parābhavato mukhaṁ.",
  "anya-e1006n0:41": "8.",
  "anya-e1006n0:42": " Pahutavitto puriso,",
  "anya-e1006n0:43": "Sahirañño sabhojano;",
  "anya-e1006n0:44": "Eko bhuñjati sādūni,",
  "anya-e1006n0:45": "Taṁ parābhavato mukhaṁ.",
  "anya-e1006n0:46": "9.",
  "anya-e1006n0:47": " Jātithaddho dhanathaddho,",
  "anya-e1006n0:48": "Gottathaddho ca yo naro;",
  "anya-e1006n0:49": "Sañātiṁ atimaññeti,",
  "anya-e1006n0:50": "Taṁ parābhavato mukhaṁ.",
  "anya-e1006n0:51": "10.",
  "anya-e1006n0:52": " Itthidhutto surādhutto,",
  "anya-e1006n0:53": "Akkhadhutto ca yo naro;",
  "anya-e1006n0:54": "Laddhaṁladdhaṁ vināseti,",
  "anya-e1006n0:55": "Taṁ parābhavato mukhaṁ.",
  "anya-e1006n0:56": "Na bāḷhaṁ itthiṁ gaccheyya,",
  "anya-e1006n0:57": "Sampassaṁ tejasaṅkhayaṁ;",
  "anya-e1006n0:58": "Kāsaṁ sāsaṁ daraṁ bālyaṁ,",
  "anya-e1006n0:59": "Khīṇamedo nigacchati.",
  "anya-e1006n0:60": "(Ka)",
  "anya-e1006n0:61": "Māyācetā marīcī ca,",
  "anya-e1006n0:62": "Soko rogo upaddavo;",
  "anya-e1006n0:63": "Kharā ca bandhanācetā,",
  "anya-e1006n0:64": "Maccupāso guhāsayo.",
  "anya-e1006n0:65": "(Kha)",
  "anya-e1006n0:66": "Balavanto dubbalā honti,",
  "anya-e1006n0:67": "Thāmavantopi hāyare;",
  "anya-e1006n0:68": "Cakkhumā andhakā honti,",
  "anya-e1006n0:69": "Mātugāmavasaṁgatā.",
  "anya-e1006n0:70": "(Ga)",
  "anya-e1006n0:71": "Guṇavanto nigguṇā honti,",
  "anya-e1006n0:72": "Paññavantopi hāyare;",
  "anya-e1006n0:73": "Pamuttā bandhanā senti,",
  "anya-e1006n0:74": "Mātugāmavasaṁgatā.",
  "anya-e1006n0:75": "(Gha)",
  "anya-e1006n0:76": "Yasaṁ kittiṁ dhitiṁ sūraṁ;",
  "anya-e1006n0:77": "Bāhussaccaṁ pajānanaṁ;",
  "anya-e1006n0:78": "Hāpayanti pamattassa;",
  "anya-e1006n0:79": "Kaṭṭhapuñcaṁva pāvako.",
  "anya-e1006n0:80": "11.",
  "anya-e1006n0:81": " Sehi dārehi santuṭṭho,",
  "anya-e1006n0:82": "Vesiyāsu padussati;",
  "anya-e1006n0:83": "Dussati paradāresu,",
  "anya-e1006n0:84": "Taṁ parābhavato mukhaṁ.",
  "anya-e1006n0:85": "(Ka)",
  "anya-e1006n0:86": "Mayañca bhariyaṁ nātikkamāma,",
  "anya-e1006n0:87": "Amheca bhariyā nātikkamanti;",
  "anya-e1006n0:88": "Aññatra tāhi brahmacariyaṁ carāma,",
  "anya-e1006n0:89": "Tasmā hi amhaṁ daharā na miyyare.",
  "anya-e1006n0:90": "(Kha)",
  "anya-e1006n0:91": "Etāsu jāyare suttamāsu,",
  "anya-e1006n0:92": "Medhāvino honti pahutapaññā;",
  "anya-e1006n0:93": "Bahussutā theraguṇā ca honti,",
  "anya-e1006n0:94": "Tasmā hi amhaṁ daharā na miyyare.",
  "anya-e1006n0:95": "12.",
  "anya-e1006n0:96": " Atītayobbano poso,",
  "anya-e1006n0:97": "Āneti timbarutthaniṁ;",
  "anya-e1006n0:98": "Tassa issā na supati,",
  "anya-e1006n0:99": "Taṁ parābhavato mukhaṁ.",
  "anya-e1006n0:100": "(Ka)",
  "anya-e1006n0:101": "Na dukkhaṁ ahinā daṭṭhaṁ,",
  "anya-e1006n0:102": "Na dukkhaṁ sattiyā hataṁ;",
  "anya-e1006n0:103": "Tañca dukkhañca tibbañca,",
  "anya-e1006n0:104": "Yaṁ passe jiṇṇakaṁ patiṁ.",
  "anya-e1006n0:105": "(Kha)",
  "anya-e1006n0:106": "Natthi khiṭṭā natthi rati,",
  "anya-e1006n0:107": "Jiṇṇena patinā saha;",
  "anya-e1006n0:108": "Natthi allāpasallāpo,",
  "anya-e1006n0:109": "Jagghituṁpi na sobhati.",
  "anya-e1006n0:110": "(Ga)",
  "anya-e1006n0:111": "Yadā ca daharo daharī,",
  "anya-e1006n0:112": "Mantayiṁsu rahogatā;",
  "anya-e1006n0:113": "Sabbe sokā vinassanti,",
  "anya-e1006n0:114": "Yekeci hadayassitā.",
  "anya-e1006n0:115": "13.",
  "anya-e1006n0:116": " Itthiṁ soṇḍiṁ vikiraṇiṁ,",
  "anya-e1006n0:117": "Purisaṁ vāpi tādisaṁ;",
  "anya-e1006n0:118": "Issariyasmiṁ thapeti,",
  "anya-e1006n0:119": "Taṁ parābhavato mukhaṁ.",
  "anya-e1006n0:120": "14.",
  "anya-e1006n0:121": " Appabhogo mahātaṇho,",
  "anya-e1006n0:122": "Khattiye jāyate kule;",
  "anya-e1006n0:123": "So ca rajjaṁ patthayati,",
  "anya-e1006n0:124": "Taṁ parābhavato mukhaṁ.",
  "anya-e1006n0:125": "‘‘Suvijāno bhavaṁhoti,",
  "anya-e1006n0:126": "Duvijāno parābhavo’’;",
  "anya-e1006n0:127": "Appabhogo mahātaṇho.",
  "anya-e1006n0:128": "15.",
  "anya-e1006n0:129": " Ete parābhave loke,",
  "anya-e1006n0:130": "Paṇḍito samavekkhiya;",
  "anya-e1006n0:131": "Ariyo dassanasampanno,",
  "anya-e1006n0:132": "Sa lokaṁ bhajate sivaṁ."
}