{
  "anya-e0601n0:0": " Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e0601n0:1": "Namakkārapāḷi",
  "anya-e0601n0:2": "1.",
  "anya-e0601n0:3": " Sugataṁ sugataṁ seṭṭhaṁ, kusalaṁkusalaṁ jahaṁ;",
  "anya-e0601n0:4": "Amataṁ amataṁ santaṁ, asamaṁ asamaṁ dadaṁ.",
  "anya-e0601n0:5": "Saraṇaṁ saraṇaṁ lokaṁ, araṇaṁ araṇaṁ karaṁ;",
  "anya-e0601n0:6": "Abhayaṁ abhayaṁ ṭhānaṁ, nāyakaṁ nāyakaṁ name.",
  "anya-e0601n0:7": "2.",
  "anya-e0601n0:8": " Nayanasubhagakāyaṅgaṁ,",
  "anya-e0601n0:9": "Madhuravarasaropetaṁ;",
  "anya-e0601n0:10": "Amitaguṇagaṇādhāraṁ,",
  "anya-e0601n0:11": "Dasabalamatulaṁ vande.",
  "anya-e0601n0:12": "3.",
  "anya-e0601n0:13": " Yo buddho dhitimāññadhārako,",
  "anya-e0601n0:14": "Saṁsāre anubhosi kāyikaṁ;",
  "anya-e0601n0:15": "Dukkhaṁ cetasikañca lokato,",
  "anya-e0601n0:16": "Taṁ vande naradevamaṅgalaṁ.",
  "anya-e0601n0:17": "4.",
  "anya-e0601n0:18": " Bāttiṁsatilakkhaṇacitradehaṁ,",
  "anya-e0601n0:19": "Dehajjutiniggatapajjalantaṁ;",
  "anya-e0601n0:20": "Paññādhitisīlaguṇoghavindaṁ,",
  "anya-e0601n0:21": "Vande munimantimajātiyuttaṁ.",
  "anya-e0601n0:22": "5.",
  "anya-e0601n0:23": " Pātodayaṁ bāladivākaraṁva,",
  "anya-e0601n0:24": "Majjhe yatīnaṁ lalitaṁ sirīhi;",
  "anya-e0601n0:25": "Puṇṇindusaṅkāsamukhaṁ anejaṁ,",
  "anya-e0601n0:26": "Vandāmi sabbaññumahaṁ munindaṁ.",
  "anya-e0601n0:27": "6.",
  "anya-e0601n0:28": " Upetapuñño varabodhimūle,",
  "anya-e0601n0:29": "Sasenamāraṁ sugato jinitvā;",
  "anya-e0601n0:30": "Abojjhi bodhiṁ aruṇodayamhi,",
  "anya-e0601n0:31": "Namāmi taṁ mārajinaṁ abhaṅgaṁ.",
  "anya-e0601n0:32": "7.",
  "anya-e0601n0:33": " Rāgādichedāmalañāṇakhaggaṁ,",
  "anya-e0601n0:34": "Satīsamaññāphalakābhigāhaṁ;",
  "anya-e0601n0:35": "Sīloghalaṅkāravibhūsitaṁ taṁ,",
  "anya-e0601n0:36": "Namāmibhiññāvaramiddhupetaṁ.",
  "anya-e0601n0:37": "8.",
  "anya-e0601n0:38": " Dayālayaṁ sabbadhi dukkaraṁ karaṁ,",
  "anya-e0601n0:39": "Bhavaṇṇavātikkamamaggataṁ gataṁ;",
  "anya-e0601n0:40": "Tilokanāthaṁ susamāhitaṁ hitaṁ,",
  "anya-e0601n0:41": "Samantacakkhuṁ paṇamāmi taṁmitaṁ.",
  "anya-e0601n0:42": "9.",
  "anya-e0601n0:43": " Tahiṁ tahiṁ pāramisañcayaṁ cayaṁ,",
  "anya-e0601n0:44": "Gataṁ gataṁ sabbhi sukhappadaṁ padaṁ;",
  "anya-e0601n0:45": "Narānarānaṁ sukhasambhavaṁ bhavaṁ,",
  "anya-e0601n0:46": "Namānamānaṁ jinapuṅgavaṁ gavaṁ.",
  "anya-e0601n0:47": "10.",
  "anya-e0601n0:48": " Maggaṅganāvaṁ munidakkhanāviko,",
  "anya-e0601n0:49": "Īhāphiyaṁ ñāṇakarena gāhako;",
  "anya-e0601n0:50": "Āruyha yo tāya bahū bhavaṇṇavā,",
  "anya-e0601n0:51": "Tāresi taṁ buddhamaghappahaṁ name.",
  "anya-e0601n0:52": "11.",
  "anya-e0601n0:53": " Samatiṁsatipāramisambharaṇaṁ,",
  "anya-e0601n0:54": "Varabodhidume catusaccadasaṁ;",
  "anya-e0601n0:55": "Varamiddhigataṁ naradevahitaṁ,",
  "anya-e0601n0:56": "Tibhavūpasamaṁ paṇamāmi jinaṁ.",
  "anya-e0601n0:57": "12.",
  "anya-e0601n0:58": " Satapuññajalakkhaṇikaṁ virajaṁ,",
  "anya-e0601n0:59": "Gaganūpamadhiṁ dhitimerusamaṁ;",
  "anya-e0601n0:60": "Jalajūpamasītalasīlayutaṁ,",
  "anya-e0601n0:61": "Pathavīsahanaṁ paṇamāmi jinaṁ.",
  "anya-e0601n0:62": "13.",
  "anya-e0601n0:63": " Yo buddho sumati dive divākarova,",
  "anya-e0601n0:64": "Sobhanto ratijanane silāsanamhi;",
  "anya-e0601n0:65": "Āsīno sivasukhadaṁ adesi dhammaṁ,",
  "anya-e0601n0:66": "Devānaṁ tamasadisaṁ namāmi niccaṁ.",
  "anya-e0601n0:67": "14.",
  "anya-e0601n0:68": " Yo pādapaṅkajamuduttalarājikehi,",
  "anya-e0601n0:69": "Lokehi tīhivikalehi nirākulehi;",
  "anya-e0601n0:70": "Sampāpuṇe nirupameyyatameva nātho,",
  "anya-e0601n0:71": "Taṁ sabbalokamahitaṁ asamaṁ namāmi.",
  "anya-e0601n0:72": "15.",
  "anya-e0601n0:73": " Buddhaṁ narānarasamosaraṇaṁ dhitattaṁ,",
  "anya-e0601n0:74": "Paññāpadīpajutiyā vihatandhakāraṁ;",
  "anya-e0601n0:75": "Atthābhikāmanaradevahitāvahaṁ taṁ,",
  "anya-e0601n0:76": "Vandāmi kāruṇikamaggamanantañāṇaṁ.",
  "anya-e0601n0:77": "16.",
  "anya-e0601n0:78": " Akhilaguṇanidhāno yo munindopagantvā,",
  "anya-e0601n0:79": "Vanamisipatanavhaṁ saññatānaṁ niketaṁ;",
  "anya-e0601n0:80": "Tahimakusalachedaṁ dhammacakkaṁ pavatto,",
  "anya-e0601n0:81": "Tamatulamabhikantaṁ vandaneyyaṁ namāmi.",
  "anya-e0601n0:82": "17.",
  "anya-e0601n0:83": " Suciparivāritaṁ surucirappabhāhi rattaṁ,",
  "anya-e0601n0:84": "Sirivisarālayaṁ gupitamindriyehupetaṁ;",
  "anya-e0601n0:85": "Ravisasimaṇḍalappabhutilakkhaṇopacittaṁ,",
  "anya-e0601n0:86": "Suranarapūjitaṁ sugatamādaraṁ namāmi.",
  "anya-e0601n0:87": "18.",
  "anya-e0601n0:88": " Maggoḷumpena muhapaṭighāsādiullolavīciṁ,",
  "anya-e0601n0:89": "Saṁsāroghaṁ tari tamabhayaṁ pārapattaṁ pajānaṁ;",
  "anya-e0601n0:90": "Tāṇaṁ leṇaṁ asamasaraṇaṁ ekatitthaṁ patiṭṭhaṁ,",
  "anya-e0601n0:91": "Puññakkhettaṁ paramasukhadaṁ dhammarājaṁ namāmi.",
  "anya-e0601n0:92": "19.",
  "anya-e0601n0:93": " Kaṇḍambaṁmūle parahitakaro yo munindo nisinno,",
  "anya-e0601n0:94": "Accheraṁ sīghaṁ nayanasubhagaṁ ākulaṇṇaggijālaṁ;",
  "anya-e0601n0:95": "Dujjāladdhaṁsaṁ munibhijahitaṁ pāṭiheraṁ akāsi,",
  "anya-e0601n0:96": "Vande taṁ seṭṭhaṁ paramaratijaṁ iddhidhammehupetaṁ.",
  "anya-e0601n0:97": "20.",
  "anya-e0601n0:98": " Munindakko yveko dayudayaruṇo ñāṇavitthiṇṇabimbo,",
  "anya-e0601n0:99": "Vineyyappāṇoghaṁ kamalakathitaṁ dhammaraṁsīvarehi;",
  "anya-e0601n0:100": "Subodhesī suddhe tibhavakuhare byāpitakkittinañca,",
  "anya-e0601n0:101": "Tilokekaccakkhuṁ dukhamasahanaṁ taṁ mahesiṁ namāmi.",
  "anya-e0601n0:102": "21.",
  "anya-e0601n0:103": " Yo jino anekajātiyaṁ saputtadāramaṅgajīvitampi,",
  "anya-e0601n0:104": "Bodhipemato alaggamānaso adāsiyeva atthikassa;",
  "anya-e0601n0:105": "Dānapāramiṁ tato paraṁ apūri sīlapāramādikampi,",
  "anya-e0601n0:106": "Tāsamiddhiyopayātamaggataṁ tamekadīpakaṁ namāmi.",
  "anya-e0601n0:107": "22.",
  "anya-e0601n0:108": " Devādevātidevaṁ nidhanavapudharaṁ mārabhaṅgaṁ abhaṅgaṁ,",
  "anya-e0601n0:109": "Dīpaṁ dīpaṁ pajānaṁ jayavarasayane bodhipattaṁdhipattaṁ;",
  "anya-e0601n0:110": "Brahmābrahmāgatānaṁ varagirakathikaṁ pāpahīnaṁ pahīnaṁ,",
  "anya-e0601n0:111": "Lokālokābhirāmaṁ satatamabhiname taṁ munindaṁ munindaṁ.",
  "anya-e0601n0:112": "23.",
  "anya-e0601n0:113": " Buddho nigrodhabimbo mudukaracaraṇo brahmaghoseṇijaṅgho,",
  "anya-e0601n0:114": "Kosacchādaṅgajāto punarapi sugato suppatiṭṭhitapādo;",
  "anya-e0601n0:115": "Mūdodātuṇṇalomo athamapi sugato brahmujuggattabhāvo,",
  "anya-e0601n0:116": "Nīlakkhī dīghapaṇhī sukhumamalachavī thomyarasaggasaggī.",
  "anya-e0601n0:117": "24.",
  "anya-e0601n0:118": " Cattālīsaggadanto samakalapanajo antaraṁsappapīno,",
  "anya-e0601n0:119": "Cakkenaṅkitapādo aviraḷadasano mārajussaṅkhapādo.",
  "anya-e0601n0:120": "Tiṭṭhanto nonamantobhayakaramudunā jaṇṇukānāmasanto,",
  "anya-e0601n0:121": "Vaṭṭakkhandho jino gotaruṇapakhumako sīhapubbaḍḍhakāyo.",
  "anya-e0601n0:122": "25.",
  "anya-e0601n0:123": " Sattappīno ca dīghaṅguli matha sugato lomakūpekalomo,",
  "anya-e0601n0:124": "Sampannodātadāṭho kanakasamataco nīlamuddhaggalomo.",
  "anya-e0601n0:125": "Sambuddho thūlajivho atha sīhahanuko jālikappādahattho,",
  "anya-e0601n0:126": "Nātho uṇhīsasīso itiguṇasahitaṁ taṁ mahesiṁ namāmi.",
  "anya-e0601n0:127": "26.",
  "anya-e0601n0:128": " Buddhobuddhotighoso atidulabhataro kā kathā buddhabhāvo,",
  "anya-e0601n0:129": "Loke tasmā vibhāvī vividhahitasukhaṁ sādhavo patthayantā.",
  "anya-e0601n0:130": "Iṭṭhaṁ atthaṁ vahantaṁ suranaramahitaṁ nibbhayaṁ dakkhiṇeyyaṁ,",
  "anya-e0601n0:131": "Lokānaṁ nandivaḍḍhaṁ dasabalamasamaṁ taṁ namassantu niccaṁ.",
  "anya-e0601n0:132": "27.",
  "anya-e0601n0:133": " Puññenetena sohaṁ nipuṇamati sato samparāye ca titto,",
  "anya-e0601n0:134": "Dakkho diṭṭhujjupañño avikalavīriyo bhogavā saṁvibhāgī.",
  "anya-e0601n0:135": "Tikkho sūro dhitatto saparahitacaro dīghajīvī arogo,",
  "anya-e0601n0:136": "Dhañño vaṇṇo yasassī atibalavadharo kittimā khantupeto.",
  "anya-e0601n0:137": "28.",
  "anya-e0601n0:138": " Saddho dātaṅgupeto paramasiridharo diṭṭhadhamme viratto,",
  "anya-e0601n0:139": "Lajjī kalyāṇamitto abhiratakusalo pañcasīlādirakkho.",
  "anya-e0601n0:140": "Appiccho appakodho ativujuhadayo iddhimā appameyyo,",
  "anya-e0601n0:141": "Pāsaṁso pemavāco sujanaguṇavidū māmako so bhaveyyaṁ.",
  "anya-e0601n0:142": "29.",
  "anya-e0601n0:143": " Itthaṁ asaṅkhaye nātha, guṇe lakkhaṇadīpite;",
  "anya-e0601n0:144": "Gāthāsu sūcakāsveka, gāthampi sarate budho.",
  "anya-e0601n0:145": "30.",
  "anya-e0601n0:146": " Caturāpāyamutto so, sādhakatthadvayassa ca;",
  "anya-e0601n0:147": "Hatūpaddavajālo ca, lābhī hitasukhassa ca.",
  "anya-e0601n0:148": "31.",
  "anya-e0601n0:149": " Adhipo naradevānaṁ, catudīpissaropi vā;",
  "anya-e0601n0:150": "Bhaveyya antime dehe, tamaññaṁ setachattakaṁ.",
  "anya-e0601n0:151": "32.",
  "anya-e0601n0:152": " Bhāvanāyānamāruyha, samamessati subbato;",
  "anya-e0601n0:153": "Imasmiṁ attabhāvepi, arogo dīghajīviko.",
  "anya-e0601n0:154": "33.",
  "anya-e0601n0:155": " Pūjito sabbalokehi, bhāvanābhiratīmano;",
  "anya-e0601n0:156": "Janappiyo manāpo ca, kā kathākhiladhāraṇe.",
  "anya-e0601n0:157": "Namakkārapāḷi niṭṭhitā."
}