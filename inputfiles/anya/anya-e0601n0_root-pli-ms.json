{
  "anya-e0601n0:0": " Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e0601n0:1": "Namakkārapāḷi",
  "anya-e0601n0:2": "1.",
  "anya-e0601n0:3": " Sugataṃ sugataṃ seṭṭhaṃ, kusalaṃkusalaṃ jahaṃ;",
  "anya-e0601n0:4": "Amataṃ amataṃ santaṃ, asamaṃ asamaṃ dadaṃ.",
  "anya-e0601n0:5": "Saraṇaṃ saraṇaṃ lokaṃ, araṇaṃ araṇaṃ karaṃ;",
  "anya-e0601n0:6": "Abhayaṃ abhayaṃ ṭhānaṃ, nāyakaṃ nāyakaṃ name.",
  "anya-e0601n0:7": "2.",
  "anya-e0601n0:8": " Nayanasubhagakāyaṅgaṃ,",
  "anya-e0601n0:9": "Madhuravarasaropetaṃ;",
  "anya-e0601n0:10": "Amitaguṇagaṇādhāraṃ,",
  "anya-e0601n0:11": "Dasabalamatulaṃ vande.",
  "anya-e0601n0:12": "3.",
  "anya-e0601n0:13": " Yo buddho dhitimāññadhārako,",
  "anya-e0601n0:14": "Saṃsāre anubhosi kāyikaṃ;",
  "anya-e0601n0:15": "Dukkhaṃ cetasikañca lokato,",
  "anya-e0601n0:16": "Taṃ vande naradevamaṅgalaṃ.",
  "anya-e0601n0:17": "4.",
  "anya-e0601n0:18": " Bāttiṃsatilakkhaṇacitradehaṃ,",
  "anya-e0601n0:19": "Dehajjutiniggatapajjalantaṃ;",
  "anya-e0601n0:20": "Paññādhitisīlaguṇoghavindaṃ,",
  "anya-e0601n0:21": "Vande munimantimajātiyuttaṃ.",
  "anya-e0601n0:22": "5.",
  "anya-e0601n0:23": " Pātodayaṃ bāladivākaraṃva,",
  "anya-e0601n0:24": "Majjhe yatīnaṃ lalitaṃ sirīhi;",
  "anya-e0601n0:25": "Puṇṇindusaṅkāsamukhaṃ anejaṃ,",
  "anya-e0601n0:26": "Vandāmi sabbaññumahaṃ munindaṃ.",
  "anya-e0601n0:27": "6.",
  "anya-e0601n0:28": " Upetapuñño varabodhimūle,",
  "anya-e0601n0:29": "Sasenamāraṃ sugato jinitvā;",
  "anya-e0601n0:30": "Abojjhi bodhiṃ aruṇodayamhi,",
  "anya-e0601n0:31": "Namāmi taṃ mārajinaṃ abhaṅgaṃ.",
  "anya-e0601n0:32": "7.",
  "anya-e0601n0:33": " Rāgādichedāmalañāṇakhaggaṃ,",
  "anya-e0601n0:34": "Satīsamaññāphalakābhigāhaṃ;",
  "anya-e0601n0:35": "Sīloghalaṅkāravibhūsitaṃ taṃ,",
  "anya-e0601n0:36": "Namāmibhiññāvaramiddhupetaṃ.",
  "anya-e0601n0:37": "8.",
  "anya-e0601n0:38": " Dayālayaṃ sabbadhi dukkaraṃ karaṃ,",
  "anya-e0601n0:39": "Bhavaṇṇavātikkamamaggataṃ gataṃ;",
  "anya-e0601n0:40": "Tilokanāthaṃ susamāhitaṃ hitaṃ,",
  "anya-e0601n0:41": "Samantacakkhuṃ paṇamāmi taṃmitaṃ.",
  "anya-e0601n0:42": "9.",
  "anya-e0601n0:43": " Tahiṃ tahiṃ pāramisañcayaṃ cayaṃ,",
  "anya-e0601n0:44": "Gataṃ gataṃ sabbhi sukhappadaṃ padaṃ;",
  "anya-e0601n0:45": "Narānarānaṃ sukhasambhavaṃ bhavaṃ,",
  "anya-e0601n0:46": "Namānamānaṃ jinapuṅgavaṃ gavaṃ.",
  "anya-e0601n0:47": "10.",
  "anya-e0601n0:48": " Maggaṅganāvaṃ munidakkhanāviko,",
  "anya-e0601n0:49": "Īhāphiyaṃ ñāṇakarena gāhako;",
  "anya-e0601n0:50": "Āruyha yo tāya bahū bhavaṇṇavā,",
  "anya-e0601n0:51": "Tāresi taṃ buddhamaghappahaṃ name.",
  "anya-e0601n0:52": "11.",
  "anya-e0601n0:53": " Samatiṃsatipāramisambharaṇaṃ,",
  "anya-e0601n0:54": "Varabodhidume catusaccadasaṃ;",
  "anya-e0601n0:55": "Varamiddhigataṃ naradevahitaṃ,",
  "anya-e0601n0:56": "Tibhavūpasamaṃ paṇamāmi jinaṃ.",
  "anya-e0601n0:57": "12.",
  "anya-e0601n0:58": " Satapuññajalakkhaṇikaṃ virajaṃ,",
  "anya-e0601n0:59": "Gaganūpamadhiṃ dhitimerusamaṃ;",
  "anya-e0601n0:60": "Jalajūpamasītalasīlayutaṃ,",
  "anya-e0601n0:61": "Pathavīsahanaṃ paṇamāmi jinaṃ.",
  "anya-e0601n0:62": "13.",
  "anya-e0601n0:63": " Yo buddho sumati dive divākarova,",
  "anya-e0601n0:64": "Sobhanto ratijanane silāsanamhi;",
  "anya-e0601n0:65": "Āsīno sivasukhadaṃ adesi dhammaṃ,",
  "anya-e0601n0:66": "Devānaṃ tamasadisaṃ namāmi niccaṃ.",
  "anya-e0601n0:67": "14.",
  "anya-e0601n0:68": " Yo pādapaṅkajamuduttalarājikehi,",
  "anya-e0601n0:69": "Lokehi tīhivikalehi nirākulehi;",
  "anya-e0601n0:70": "Sampāpuṇe nirupameyyatameva nātho,",
  "anya-e0601n0:71": "Taṃ sabbalokamahitaṃ asamaṃ namāmi.",
  "anya-e0601n0:72": "15.",
  "anya-e0601n0:73": " Buddhaṃ narānarasamosaraṇaṃ dhitattaṃ,",
  "anya-e0601n0:74": "Paññāpadīpajutiyā vihatandhakāraṃ;",
  "anya-e0601n0:75": "Atthābhikāmanaradevahitāvahaṃ taṃ,",
  "anya-e0601n0:76": "Vandāmi kāruṇikamaggamanantañāṇaṃ.",
  "anya-e0601n0:77": "16.",
  "anya-e0601n0:78": " Akhilaguṇanidhāno yo munindopagantvā,",
  "anya-e0601n0:79": "Vanamisipatanavhaṃ saññatānaṃ niketaṃ;",
  "anya-e0601n0:80": "Tahimakusalachedaṃ dhammacakkaṃ pavatto,",
  "anya-e0601n0:81": "Tamatulamabhikantaṃ vandaneyyaṃ namāmi.",
  "anya-e0601n0:82": "17.",
  "anya-e0601n0:83": " Suciparivāritaṃ surucirappabhāhi rattaṃ,",
  "anya-e0601n0:84": "Sirivisarālayaṃ gupitamindriyehupetaṃ;",
  "anya-e0601n0:85": "Ravisasimaṇḍalappabhutilakkhaṇopacittaṃ,",
  "anya-e0601n0:86": "Suranarapūjitaṃ sugatamādaraṃ namāmi.",
  "anya-e0601n0:87": "18.",
  "anya-e0601n0:88": " Maggoḷumpena muhapaṭighāsādiullolavīciṃ,",
  "anya-e0601n0:89": "Saṃsāroghaṃ tari tamabhayaṃ pārapattaṃ pajānaṃ;",
  "anya-e0601n0:90": "Tāṇaṃ leṇaṃ asamasaraṇaṃ ekatitthaṃ patiṭṭhaṃ,",
  "anya-e0601n0:91": "Puññakkhettaṃ paramasukhadaṃ dhammarājaṃ namāmi.",
  "anya-e0601n0:92": "19.",
  "anya-e0601n0:93": " Kaṇḍambaṃmūle parahitakaro yo munindo nisinno,",
  "anya-e0601n0:94": "Accheraṃ sīghaṃ nayanasubhagaṃ ākulaṇṇaggijālaṃ;",
  "anya-e0601n0:95": "Dujjāladdhaṃsaṃ munibhijahitaṃ pāṭiheraṃ akāsi,",
  "anya-e0601n0:96": "Vande taṃ seṭṭhaṃ paramaratijaṃ iddhidhammehupetaṃ.",
  "anya-e0601n0:97": "20.",
  "anya-e0601n0:98": " Munindakko yveko dayudayaruṇo ñāṇavitthiṇṇabimbo,",
  "anya-e0601n0:99": "Vineyyappāṇoghaṃ kamalakathitaṃ dhammaraṃsīvarehi;",
  "anya-e0601n0:100": "Subodhesī suddhe tibhavakuhare byāpitakkittinañca,",
  "anya-e0601n0:101": "Tilokekaccakkhuṃ dukhamasahanaṃ taṃ mahesiṃ namāmi.",
  "anya-e0601n0:102": "21.",
  "anya-e0601n0:103": " Yo jino anekajātiyaṃ saputtadāramaṅgajīvitampi,",
  "anya-e0601n0:104": "Bodhipemato alaggamānaso adāsiyeva atthikassa;",
  "anya-e0601n0:105": "Dānapāramiṃ tato paraṃ apūri sīlapāramādikampi,",
  "anya-e0601n0:106": "Tāsamiddhiyopayātamaggataṃ tamekadīpakaṃ namāmi.",
  "anya-e0601n0:107": "22.",
  "anya-e0601n0:108": " Devādevātidevaṃ nidhanavapudharaṃ mārabhaṅgaṃ abhaṅgaṃ,",
  "anya-e0601n0:109": "Dīpaṃ dīpaṃ pajānaṃ jayavarasayane bodhipattaṃdhipattaṃ;",
  "anya-e0601n0:110": "Brahmābrahmāgatānaṃ varagirakathikaṃ pāpahīnaṃ pahīnaṃ,",
  "anya-e0601n0:111": "Lokālokābhirāmaṃ satatamabhiname taṃ munindaṃ munindaṃ.",
  "anya-e0601n0:112": "23.",
  "anya-e0601n0:113": " Buddho nigrodhabimbo mudukaracaraṇo brahmaghoseṇijaṅgho,",
  "anya-e0601n0:114": "Kosacchādaṅgajāto punarapi sugato suppatiṭṭhitapādo;",
  "anya-e0601n0:115": "Mūdodātuṇṇalomo athamapi sugato brahmujuggattabhāvo,",
  "anya-e0601n0:116": "Nīlakkhī dīghapaṇhī sukhumamalachavī thomyarasaggasaggī.",
  "anya-e0601n0:117": "24.",
  "anya-e0601n0:118": " Cattālīsaggadanto samakalapanajo antaraṃsappapīno,",
  "anya-e0601n0:119": "Cakkenaṅkitapādo aviraḷadasano mārajussaṅkhapādo.",
  "anya-e0601n0:120": "Tiṭṭhanto nonamantobhayakaramudunā jaṇṇukānāmasanto,",
  "anya-e0601n0:121": "Vaṭṭakkhandho jino gotaruṇapakhumako sīhapubbaḍḍhakāyo.",
  "anya-e0601n0:122": "25.",
  "anya-e0601n0:123": " Sattappīno ca dīghaṅguli matha sugato lomakūpekalomo,",
  "anya-e0601n0:124": "Sampannodātadāṭho kanakasamataco nīlamuddhaggalomo.",
  "anya-e0601n0:125": "Sambuddho thūlajivho atha sīhahanuko jālikappādahattho,",
  "anya-e0601n0:126": "Nātho uṇhīsasīso itiguṇasahitaṃ taṃ mahesiṃ namāmi.",
  "anya-e0601n0:127": "26.",
  "anya-e0601n0:128": " Buddhobuddhotighoso atidulabhataro kā kathā buddhabhāvo,",
  "anya-e0601n0:129": "Loke tasmā vibhāvī vividhahitasukhaṃ sādhavo patthayantā.",
  "anya-e0601n0:130": "Iṭṭhaṃ atthaṃ vahantaṃ suranaramahitaṃ nibbhayaṃ dakkhiṇeyyaṃ,",
  "anya-e0601n0:131": "Lokānaṃ nandivaḍḍhaṃ dasabalamasamaṃ taṃ namassantu niccaṃ.",
  "anya-e0601n0:132": "27.",
  "anya-e0601n0:133": " Puññenetena sohaṃ nipuṇamati sato samparāye ca titto,",
  "anya-e0601n0:134": "Dakkho diṭṭhujjupañño avikalavīriyo bhogavā saṃvibhāgī.",
  "anya-e0601n0:135": "Tikkho sūro dhitatto saparahitacaro dīghajīvī arogo,",
  "anya-e0601n0:136": "Dhañño vaṇṇo yasassī atibalavadharo kittimā khantupeto.",
  "anya-e0601n0:137": "28.",
  "anya-e0601n0:138": " Saddho dātaṅgupeto paramasiridharo diṭṭhadhamme viratto,",
  "anya-e0601n0:139": "Lajjī kalyāṇamitto abhiratakusalo pañcasīlādirakkho.",
  "anya-e0601n0:140": "Appiccho appakodho ativujuhadayo iddhimā appameyyo,",
  "anya-e0601n0:141": "Pāsaṃso pemavāco sujanaguṇavidū māmako so bhaveyyaṃ.",
  "anya-e0601n0:142": "29.",
  "anya-e0601n0:143": " Itthaṃ asaṅkhaye nātha, guṇe lakkhaṇadīpite;",
  "anya-e0601n0:144": "Gāthāsu sūcakāsveka, gāthampi sarate budho.",
  "anya-e0601n0:145": "30.",
  "anya-e0601n0:146": " Caturāpāyamutto so, sādhakatthadvayassa ca;",
  "anya-e0601n0:147": "Hatūpaddavajālo ca, lābhī hitasukhassa ca.",
  "anya-e0601n0:148": "31.",
  "anya-e0601n0:149": " Adhipo naradevānaṃ, catudīpissaropi vā;",
  "anya-e0601n0:150": "Bhaveyya antime dehe, tamaññaṃ setachattakaṃ.",
  "anya-e0601n0:151": "32.",
  "anya-e0601n0:152": " Bhāvanāyānamāruyha, samamessati subbato;",
  "anya-e0601n0:153": "Imasmiṃ attabhāvepi, arogo dīghajīviko.",
  "anya-e0601n0:154": "33.",
  "anya-e0601n0:155": " Pūjito sabbalokehi, bhāvanābhiratīmano;",
  "anya-e0601n0:156": "Janappiyo manāpo ca, kā kathākhiladhāraṇe.",
  "anya-e0601n0:157": "Namakkārapāḷi niṭṭhitā."
}