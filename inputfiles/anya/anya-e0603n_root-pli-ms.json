{
  "anya-e0603n:0": " Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e0603n:1": "Lakkhaṇāto",
  "anya-e0603n:2": "Buddhathomanāgāthā",
  "anya-e0603n:3": "1.",
  "anya-e0603n:4": " Bhavābhavesu nekesu,",
  "anya-e0603n:5": "Pūresi tiṃsapāramī;",
  "anya-e0603n:6": "Cariyāyoca sambuddho,",
  "anya-e0603n:7": "Pariccāgejahaṃsadā.",
  "anya-e0603n:8": "2.",
  "anya-e0603n:9": " Vessantarattabhāvamhi,",
  "anya-e0603n:10": "Ṭhitomaddiṃ pisoadā;",
  "anya-e0603n:11": "Nijaṃkaṇhājinaṃjāliṃ,",
  "anya-e0603n:12": "Viyaṃvaṅkatapabbate.",
  "anya-e0603n:13": "3.",
  "anya-e0603n:14": " Tatocavitvātusite,",
  "anya-e0603n:15": "Pureuppajjipuññavā;",
  "anya-e0603n:16": "Rūpādidasaṭhānehi,",
  "anya-e0603n:17": "Aññedeveatikkami.",
  "anya-e0603n:18": "4.",
  "anya-e0603n:19": " Cakkavāḷasahassehi,",
  "anya-e0603n:20": "Dasahāgammasabbaso;",
  "anya-e0603n:21": "Yācitodevabrahmūhi,",
  "anya-e0603n:22": "Buddhabhāvamanonato.",
  "anya-e0603n:23": "5.",
  "anya-e0603n:24": " Vilokanānisopañca,",
  "anya-e0603n:25": "Viloketvāsirindharo;",
  "anya-e0603n:26": "Tatocavitvāmāyāya,",
  "anya-e0603n:27": "Uppajjisākiyekule.",
  "anya-e0603n:28": "6.",
  "anya-e0603n:29": " Visākhapuṇṇameseṭṭhe,",
  "anya-e0603n:30": "Vijāyilumbinīvane;",
  "anya-e0603n:31": "Sīhanādaṃpanādento,",
  "anya-e0603n:32": "Sattapādaṃgatotadā.",
  "anya-e0603n:33": "8.",
  "anya-e0603n:34": " Virociṃsutadākāse,",
  "anya-e0603n:35": "Tārocandadivākarā;",
  "anya-e0603n:36": "Ratanānicabhūmaṭṭhā,",
  "anya-e0603n:37": "Nānāpupphācapupphare.",
  "anya-e0603n:38": "8.",
  "anya-e0603n:39": " Sītāyanti avīcaggī,",
  "anya-e0603n:40": "Khuppipāsācasohitā;",
  "anya-e0603n:41": "Lokantaremahāpabhā,",
  "anya-e0603n:42": "Andhāpassantitāvade.",
  "anya-e0603n:43": "9.",
  "anya-e0603n:44": " Aghaṭṭitāvāditāni,",
  "anya-e0603n:45": "Tūriyānicaambare;",
  "anya-e0603n:46": "Madhurenapanādiṃsu,",
  "anya-e0603n:47": "Yasaṃsarenatāvade.",
  "anya-e0603n:48": "10.",
  "anya-e0603n:49": " Thutigītānigāyantā,",
  "anya-e0603n:50": "Devācakamalāsanā;",
  "anya-e0603n:51": "Sammodentāvaaṭṭhaṃsu,",
  "anya-e0603n:52": "Buddhaṅkurassasantike.",
  "anya-e0603n:53": "11.",
  "anya-e0603n:54": " Kumārocārudhātīhi,",
  "anya-e0603n:55": "Tositārājamandire;",
  "anya-e0603n:56": "Vuddhosisusukhaṃkhemaṃ,",
  "anya-e0603n:57": "Sakyakaññānamantare.",
  "anya-e0603n:58": "12.",
  "anya-e0603n:59": " Soḷasavassikosakya,",
  "anya-e0603n:60": "Nandanopitarāsatā;",
  "anya-e0603n:61": "Rājatteabhisittosi,",
  "anya-e0603n:62": "Modesituvanattaye.",
  "anya-e0603n:63": "13.",
  "anya-e0603n:64": " Nandanānaṃsovaṇṇa,",
  "anya-e0603n:65": "Vaṇṇasobhaṃyasodharaṃ;",
  "anya-e0603n:66": "Sokāresimahesittaṃ,",
  "anya-e0603n:67": "Lokanettarasāyanaṃ.",
  "anya-e0603n:68": "14.",
  "anya-e0603n:69": " Ekūnatiṃsamevasse,",
  "anya-e0603n:70": "Visākhapuṇṇamevare;",
  "anya-e0603n:71": "Sahasenāyauyyāsi,",
  "anya-e0603n:72": "Yānenuyyānamaṇḍalaṃ.",
  "anya-e0603n:73": "15.",
  "anya-e0603n:74": " Nimittecaturodisvā,",
  "anya-e0603n:75": "Puttassacavijāyanaṃ;",
  "anya-e0603n:76": "Sutvāubbiggacittena,",
  "anya-e0603n:77": "Naramitthapuretahiṃ.",
  "anya-e0603n:78": "16.",
  "anya-e0603n:79": " Paccāgammapuraṃtamhā,",
  "anya-e0603n:80": "Pattopāsādamaṇḍalaṃ;",
  "anya-e0603n:81": "Nippajjidevarājāva,",
  "anya-e0603n:82": "Sayanesomahārahe.",
  "anya-e0603n:83": "17.",
  "anya-e0603n:84": " Sundarītaṃpurakkhitvā,",
  "anya-e0603n:85": "Naccagītenatosayuṃ;",
  "anya-e0603n:86": "Evaṃpisonaramitvā,",
  "anya-e0603n:87": "Āhunikkhamamānaso.",
  "anya-e0603n:88": "18.",
  "anya-e0603n:89": " Pathamaṃrāhulaṃputtaṃ,",
  "anya-e0603n:90": "Passitvānikkhamāmahaṃ;",
  "anya-e0603n:91": "Iticintiyamāneso,",
  "anya-e0603n:92": "Agājāyānivesanaṃ;",
  "anya-e0603n:93": "19.",
  "anya-e0603n:94": " Thapetvāpādadummāre,",
  "anya-e0603n:95": "Gīvamantopavesayaṃ;",
  "anya-e0603n:96": "Olokayaṃsapassittha,",
  "anya-e0603n:97": "Nippannaṃtaṃsamātaraṃ.",
  "anya-e0603n:98": "20.",
  "anya-e0603n:99": " Gaṇheyyaṃtaṃapanetvā,",
  "anya-e0603n:100": "Bāhuṃcedeviyāahaṃ;",
  "anya-e0603n:101": "Nadadenikkhamokāsaṃ,",
  "anya-e0603n:102": "Pabujjhitvāyasodharā.",
  "anya-e0603n:103": "21.",
  "anya-e0603n:104": " Buddhapattovapasseyyaṃ,",
  "anya-e0603n:105": "Rāhulaṃiticintiya;",
  "anya-e0603n:106": "Kaṇḍakaṃāruhitvāna,",
  "anya-e0603n:107": "Tamhāchannenanikkhami.",
  "anya-e0603n:108": "22.",
  "anya-e0603n:109": " Dvārāsayāvivariṃsu,",
  "anya-e0603n:110": "Devatādvāramaṇḍalaṃ;",
  "anya-e0603n:111": "Ratanukkāsahassāni,",
  "anya-e0603n:112": "Jālayiṃsumarūtahiṃ.",
  "anya-e0603n:113": "23.",
  "anya-e0603n:114": " Mānikkhamassusiddhattha,",
  "anya-e0603n:115": "Itotvaṃsattamedine;",
  "anya-e0603n:116": "Bhavissasecakkavattī,",
  "anya-e0603n:117": "Itinīvāripāpimā.",
  "anya-e0603n:118": "24.",
  "anya-e0603n:119": " Tagghajānāmitaṃmāra,",
  "anya-e0603n:120": "Idhamātiṭṭhapakkama;",
  "anya-e0603n:121": "Itimāraṃ palāpetvā,",
  "anya-e0603n:122": "Vissaṭṭhosoabhikkami.",
  "anya-e0603n:123": "25.",
  "anya-e0603n:124": " Patvānomanadītīraṃ,",
  "anya-e0603n:125": "Punāgamesituragaṃ;",
  "anya-e0603n:126": "Channañcanagaraṃ bimbā;",
  "anya-e0603n:127": "Devilāpaṭivedituṃ.",
  "anya-e0603n:128": "26.",
  "anya-e0603n:129": " Bandhitaṃyasodharāya,",
  "anya-e0603n:130": "Moḷiṃsugandhavāsitaṃ;",
  "anya-e0603n:131": "Chetvākhaggenatiṇhena,",
  "anya-e0603n:132": "Ukkhipitthatamambare.",
  "anya-e0603n:133": "27.",
  "anya-e0603n:134": " Aṭṭhaseṭṭhaparikkhāre,",
  "anya-e0603n:135": "Dhāretvābrahmunābhate;",
  "anya-e0603n:136": "Hitvārājārahaṃdussaṃ,",
  "anya-e0603n:137": "Samaṇatthamupāgami.",
  "anya-e0603n:138": "28.",
  "anya-e0603n:139": " Karontodukkaraṃkammaṃ,",
  "anya-e0603n:140": "Chabbassāniparakkamo;",
  "anya-e0603n:141": "Oḷārikannapānehi;",
  "anya-e0603n:142": "Dehaṃyāpesiṭhītiyā.",
  "anya-e0603n:143": "29.",
  "anya-e0603n:144": " Visākhapuṇṇamehema,",
  "anya-e0603n:145": "Pātiṃpāyasapūritaṃ;",
  "anya-e0603n:146": "Sujātāyābhataṃgayha,",
  "anya-e0603n:147": "Nerañcaramupāgami.",
  "anya-e0603n:148": "30.",
  "anya-e0603n:149": " Nerañcarāyatīramhi,",
  "anya-e0603n:150": "Sutvāpāyāsamuttamaṃ;",
  "anya-e0603n:151": "Paṭiyatthavaramaggena,",
  "anya-e0603n:152": "Bodhimaṇḍamupāgami.",
  "anya-e0603n:153": "31.",
  "anya-e0603n:154": " Jayāsanepallaṅkamhi,",
  "anya-e0603n:155": "Ṭhitodhiṭṭhesidhītimā;",
  "anya-e0603n:156": "Navīriyaṃjahissāmi,",
  "anya-e0603n:157": "Apattoitibuddhataṃ.",
  "anya-e0603n:158": "32.",
  "anya-e0603n:159": " Devindodhamayaṃsaṅkhaṃ,",
  "anya-e0603n:160": "Setacchattaṃpitāmaho;",
  "anya-e0603n:161": "Dhārayaṃvādentovīṇaṃ,",
  "anya-e0603n:162": "Pañcasīkhoṭhitotahiṃ.",
  "anya-e0603n:163": "33.",
  "anya-e0603n:164": " Thutigītānigāyantī,",
  "anya-e0603n:165": "Aṭṭhāsuṃdevasundarī;",
  "anya-e0603n:166": "Gahetvāhemamañjūsā,",
  "anya-e0603n:167": "Surapupphehipūritā.",
  "anya-e0603n:168": "34.",
  "anya-e0603n:169": " Anatthamentesūriye,",
  "anya-e0603n:170": "Mārasenaṃpalāpayī;",
  "anya-e0603n:171": "Aruṇuggamanekāle,",
  "anya-e0603n:172": "Pattosibuddhataṃvaraṃ.",
  "anya-e0603n:173": "35.",
  "anya-e0603n:174": " Paṭṭhānaṃsammasantassa,",
  "anya-e0603n:175": "Nāthassadehatosubhā;",
  "anya-e0603n:176": "Chabbaṇṇabhānicchadhanti,",
  "anya-e0603n:177": "Vidhāvanti tahiṃ tahiṃ.",
  "anya-e0603n:178": "36.",
  "anya-e0603n:179": " Nīlānīlāpitāpītā,",
  "anya-e0603n:180": "Rattārattācanicchare;",
  "anya-e0603n:181": "Setāsetācamañciṭṭhā,",
  "anya-e0603n:182": "Tamhātamhāpabhassarā.",
  "anya-e0603n:183": "37.",
  "anya-e0603n:184": " Buddhapattopibodheyya,",
  "anya-e0603n:185": "Dhammagambhīrataṃjino;",
  "anya-e0603n:186": "Nisāmetvānadesetuṃ,",
  "anya-e0603n:187": "Nirussāhamupāgato.",
  "anya-e0603n:188": "38.",
  "anya-e0603n:189": " Sahampatitadābrahmā,",
  "anya-e0603n:190": "Desanāyābhiyācati;",
  "anya-e0603n:191": "Tenasopathamaṃseṭṭhaṃ,",
  "anya-e0603n:192": "Dhammacakkamadesayi.",
  "anya-e0603n:193": "39.",
  "anya-e0603n:194": " Ekavassovasambuddho,",
  "anya-e0603n:195": "Varephagguṇapuṇṇame;",
  "anya-e0603n:196": "Sakyānaṃnagaraṃgañchī,",
  "anya-e0603n:197": "Sāvakehipurakkhato.",
  "anya-e0603n:198": "40.",
  "anya-e0603n:199": " Samāgamamhiñātīnaṃ,",
  "anya-e0603n:200": "Vassāpesisusītalaṃ;",
  "anya-e0603n:201": "Mahāmeghañcadesesi,",
  "anya-e0603n:202": "Vessantarassajātakaṃ.",
  "anya-e0603n:203": "41.",
  "anya-e0603n:204": " Gantvāsāvakayugena,",
  "anya-e0603n:205": "Yasodharāyamandiraṃ;",
  "anya-e0603n:206": "Taṃtosetvānadesesi,",
  "anya-e0603n:207": "Candakinnarajātakaṃ.",
  "anya-e0603n:208": "42.",
  "anya-e0603n:209": " Vasitvāsattamevasse,",
  "anya-e0603n:210": "Tāvatiṃse silāsane;",
  "anya-e0603n:211": "Abhidhammena tosesi,",
  "anya-e0603n:212": "Devabrahmagaṇaṃbahuṃ.",
  "anya-e0603n:213": "43.",
  "anya-e0603n:214": " Pattharaṃñāṇajālena,",
  "anya-e0603n:215": "Lokaṃlokesisabbadā;",
  "anya-e0603n:216": "Bodhetuṃbodhiyābhāya;",
  "anya-e0603n:217": "Bodheyyajanapaṅkajaṃ.",
  "anya-e0603n:218": "44.",
  "anya-e0603n:219": " Bodhaneyyaṃjanaṃdisvā,",
  "anya-e0603n:220": "Cakkavāḷaparaṃparā;",
  "anya-e0603n:221": "Gantvāpivibodhesi,",
  "anya-e0603n:222": "Buddhaseṭṭhosubodhimā.",
  "anya-e0603n:223": "45.",
  "anya-e0603n:224": " Bakādikecabrahmāno,",
  "anya-e0603n:225": "Yakkhecāḷavakādike;",
  "anya-e0603n:226": "Purindadādikedeve,",
  "anya-e0603n:227": "Sūcilomādirakkhase.",
  "anya-e0603n:228": "46.",
  "anya-e0603n:229": " Jāṇusoṇādikeneke,",
  "anya-e0603n:230": "Brāhmaṇecāvikakkhaḷe;",
  "anya-e0603n:231": "Cūḷodarādikenāge,",
  "anya-e0603n:232": "Dhanapālādikegaje.",
  "anya-e0603n:233": "47.",
  "anya-e0603n:234": " Buddhāmayaṃtimaññante,",
  "anya-e0603n:235": "Titthiyenekaladdhike;",
  "anya-e0603n:236": "Damesidamataṃseṭṭho,",
  "anya-e0603n:237": "Paravādappamaddano.",
  "anya-e0603n:238": "48.",
  "anya-e0603n:239": " Anubyañjanasampanno,",
  "anya-e0603n:240": "Esabāttiṃsalakkhaṇo;",
  "anya-e0603n:241": "Byāmappabhāparikkhitto,",
  "anya-e0603n:242": "Ketumālāsilaṅkhato.",
  "anya-e0603n:243": "49.",
  "anya-e0603n:244": " Caturāsītisahassa,",
  "anya-e0603n:245": "Dhammakkhandhesadesayi;",
  "anya-e0603n:246": "Pañcatāthīsavassāni,",
  "anya-e0603n:247": "Tilokassahitāyave.",
  "anya-e0603n:248": "50.",
  "anya-e0603n:249": " Buddhakiccaṃkarontoso,",
  "anya-e0603n:250": "Vahaṃlokahitaṃvaraṃ;",
  "anya-e0603n:251": "Mocayaṃbandhanāsatte,",
  "anya-e0603n:252": "Kālaṃkhepetisabbadā.",
  "anya-e0603n:253": "51.",
  "anya-e0603n:254": " Yahiṃ āsītikonātho,",
  "anya-e0603n:255": "Nibbutovaramañjake;",
  "anya-e0603n:256": "Mahāsālavaneramme,",
  "anya-e0603n:257": "Mallānaṃnigametahiṃ.",
  "anya-e0603n:258": "52.",
  "anya-e0603n:259": " Aggikkhandhovalokasmiṃ,",
  "anya-e0603n:260": "Jāletvānasasāvako;",
  "anya-e0603n:261": "Nibbāyitthasaṅkhatānaṃ,",
  "anya-e0603n:262": "Pakāsentoaniccataṃti.",
  "anya-e0603n:263": "‘‘Namotebhagavānātha,",
  "anya-e0603n:264": "Namotejayataṃjaya;",
  "anya-e0603n:265": "Namotesirisampanna,",
  "anya-e0603n:266": "Namotelokanandana.’’",
  "anya-e0603n:267": "Buddhavandanā",
  "anya-e0603n:268": "1.",
  "anya-e0603n:269": " Devalokā cavitvāna,",
  "anya-e0603n:270": "Mahāmāyāya kucchiyaṃ;",
  "anya-e0603n:271": "Uppajji guruvāramhi,",
  "anya-e0603n:272": "Vandetaṃ sakyapuṅgavaṃ.",
  "anya-e0603n:273": "2.",
  "anya-e0603n:274": " Dasamā saccaye neso,",
  "anya-e0603n:275": "Vijāyi mātukucchito;",
  "anya-e0603n:276": "Sukkavāre lumbiniyaṃ,",
  "anya-e0603n:277": "Vande taṃ lokapūjitaṃ.",
  "anya-e0603n:278": "3.",
  "anya-e0603n:279": " Cakkavattisiriṃ hitvā,",
  "anya-e0603n:280": "Mahāsinikkhamaṃ sudhī;",
  "anya-e0603n:281": "Nikkhamī candavāramhi,",
  "anya-e0603n:282": "Name taṃ munikuñjaraṃ.",
  "anya-e0603n:283": "4.",
  "anya-e0603n:284": " Assatthabodhimūlamhi,",
  "anya-e0603n:285": "Pallaṅke aparājite;",
  "anya-e0603n:286": "Patto sabbaññutaṃ nātho,",
  "anya-e0603n:287": "Name taṃ budhavāsare.",
  "anya-e0603n:288": "5.",
  "anya-e0603n:289": " Pañcavaggiyamukhānaṃ,",
  "anya-e0603n:290": "Devānaṃ migadāvane;",
  "anya-e0603n:291": "Sorivāre dhammacakkaṃ,",
  "anya-e0603n:292": "Vattesi taṃ jinaṃ name.",
  "anya-e0603n:293": "6.",
  "anya-e0603n:294": " Sabbasaṅkhatadhammānaṃ,",
  "anya-e0603n:295": "Pakāsiyaaniccataṃ;",
  "anya-e0603n:296": "Nibbutaṃaṅgavāramhi,",
  "anya-e0603n:297": "Namādhiamatandadaṃ.",
  "anya-e0603n:298": "7.",
  "anya-e0603n:299": " Sūriyavāre soṇṇāya,",
  "anya-e0603n:300": "Doṇiyā jinaviggaho;",
  "anya-e0603n:301": "Agginujjalito tassa,",
  "anya-e0603n:302": "Name dhātusarīrakaṃ.",
  "anya-e0603n:303": "Uṇṇālomikanātha vandanā",
  "anya-e0603n:304": "Uṇṇālodhikanāthassa,",
  "anya-e0603n:305": "Uṇṇāya bhamukantare;",
  "anya-e0603n:306": "Vajirāviya sobhanti,",
  "anya-e0603n:307": "Nikkhanti yo supaṇḍarā.",
  "anya-e0603n:308": "Vajiragghanakāyassa,",
  "anya-e0603n:309": "Nāthassa dehato subhā;",
  "anya-e0603n:310": "Vajireyyā niccharanti;",
  "anya-e0603n:311": "Vijjūva gagaṇantare.",
  "anya-e0603n:312": "Rinindamandahāsena,",
  "anya-e0603n:313": "Dāṭhāya niccharā pabhā;",
  "anya-e0603n:314": "Vipphūrisā disāsabbā,",
  "anya-e0603n:315": "Vajirāyanti paṇḍarā."
}