{
  "anya-e0603n:6": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e0603n:7": "Lakkhaṇāto",
  "anya-e0603n:8": "Buddhathomanāgāthā",
  "anya-e0603n:9": "1.",
  "anya-e0603n:10": "Bhavābhavesu nekesu,",
  "anya-e0603n:11": "Pūresi tiṃsapāramī;",
  "anya-e0603n:12": "Cariyāyoca sambuddho,",
  "anya-e0603n:13": "Pariccāgejahaṃsadā.",
  "anya-e0603n:14": "2.",
  "anya-e0603n:15": "Vessantarattabhāvamhi,",
  "anya-e0603n:16": "Ṭhitomaddiṃ pisoadā;",
  "anya-e0603n:17": "Nijaṃkaṇhājinaṃjāliṃ,",
  "anya-e0603n:18": "Viyaṃvaṅkatapabbate.",
  "anya-e0603n:19": "3.",
  "anya-e0603n:20": "Tatocavitvātusite,",
  "anya-e0603n:21": "Pureuppajjipuññavā;",
  "anya-e0603n:22": "Rūpādidasaṭhānehi,",
  "anya-e0603n:23": "Aññedeveatikkami.",
  "anya-e0603n:24": "4.",
  "anya-e0603n:25": "Cakkavāḷasahassehi,",
  "anya-e0603n:26": "Dasahāgammasabbaso;",
  "anya-e0603n:27": "Yācitodevabrahmūhi,",
  "anya-e0603n:28": "Buddhabhāvamanonato.",
  "anya-e0603n:29": "5.",
  "anya-e0603n:30": "Vilokanānisopañca,",
  "anya-e0603n:31": "Viloketvāsirindharo;",
  "anya-e0603n:32": "Tatocavitvāmāyāya,",
  "anya-e0603n:33": "Uppajjisākiyekule.",
  "anya-e0603n:34": "6.",
  "anya-e0603n:35": "Visākhapuṇṇameseṭṭhe,",
  "anya-e0603n:36": "Vijāyilumbinīvane;",
  "anya-e0603n:37": "Sīhanādaṃpanādento,",
  "anya-e0603n:38": "Sattapādaṃgatotadā.",
  "anya-e0603n:39": "8.",
  "anya-e0603n:40": "Virociṃsutadākāse,",
  "anya-e0603n:41": "Tārocandadivākarā;",
  "anya-e0603n:42": "Ratanānicabhūmaṭṭhā,",
  "anya-e0603n:43": "Nānāpupphācapupphare.",
  "anya-e0603n:44": "8.",
  "anya-e0603n:45": "Sītāyanti avīcaggī,",
  "anya-e0603n:46": "Khuppipāsācasohitā;",
  "anya-e0603n:47": "Lokantaremahāpabhā,",
  "anya-e0603n:48": "Andhāpassantitāvade.",
  "anya-e0603n:49": "9.",
  "anya-e0603n:50": "Aghaṭṭitāvāditāni,",
  "anya-e0603n:51": "Tūriyānicaambare;",
  "anya-e0603n:52": "Madhurenapanādiṃsu,",
  "anya-e0603n:53": "Yasaṃsarenatāvade.",
  "anya-e0603n:54": "10.",
  "anya-e0603n:55": "Thutigītānigāyantā,",
  "anya-e0603n:56": "Devācakamalāsanā;",
  "anya-e0603n:57": "Sammodentāvaaṭṭhaṃsu,",
  "anya-e0603n:58": "Buddhaṅkurassasantike.",
  "anya-e0603n:59": "11.",
  "anya-e0603n:60": "Kumārocārudhātīhi,",
  "anya-e0603n:61": "Tositārājamandire;",
  "anya-e0603n:62": "Vuddhosisusukhaṃkhemaṃ,",
  "anya-e0603n:63": "Sakyakaññānamantare.",
  "anya-e0603n:64": "12.",
  "anya-e0603n:65": "Soḷasavassikosakya,",
  "anya-e0603n:66": "Nandanopitarāsatā;",
  "anya-e0603n:67": "Rājatteabhisittosi,",
  "anya-e0603n:68": "Modesituvanattaye.",
  "anya-e0603n:69": "13.",
  "anya-e0603n:70": "Nandanānaṃsovaṇṇa,",
  "anya-e0603n:71": "Vaṇṇasobhaṃyasodharaṃ;",
  "anya-e0603n:72": "Sokāresimahesittaṃ,",
  "anya-e0603n:73": "Lokanettarasāyanaṃ.",
  "anya-e0603n:74": "14.",
  "anya-e0603n:75": "Ekūnatiṃsamevasse,",
  "anya-e0603n:76": "Visākhapuṇṇamevare;",
  "anya-e0603n:77": "Sahasenāyauyyāsi,",
  "anya-e0603n:78": "Yānenuyyānamaṇḍalaṃ.",
  "anya-e0603n:79": "15.",
  "anya-e0603n:80": "Nimittecaturodisvā,",
  "anya-e0603n:81": "Puttassacavijāyanaṃ;",
  "anya-e0603n:82": "Sutvāubbiggacittena,",
  "anya-e0603n:83": "Naramitthapuretahiṃ.",
  "anya-e0603n:84": "16.",
  "anya-e0603n:85": "Paccāgammapuraṃtamhā,",
  "anya-e0603n:86": "Pattopāsādamaṇḍalaṃ;",
  "anya-e0603n:87": "Nippajjidevarājāva,",
  "anya-e0603n:88": "Sayanesomahārahe.",
  "anya-e0603n:89": "17.",
  "anya-e0603n:90": "Sundarītaṃpurakkhitvā,",
  "anya-e0603n:91": "Naccagītenatosayuṃ;",
  "anya-e0603n:92": "Evaṃpisonaramitvā,",
  "anya-e0603n:93": "Āhunikkhamamānaso.",
  "anya-e0603n:94": "18.",
  "anya-e0603n:95": "Pathamaṃrāhulaṃputtaṃ,",
  "anya-e0603n:96": "Passitvānikkhamāmahaṃ;",
  "anya-e0603n:97": "Iticintiyamāneso,",
  "anya-e0603n:98": "Agājāyānivesanaṃ;",
  "anya-e0603n:99": "19.",
  "anya-e0603n:100": "Thapetvāpādadummāre,",
  "anya-e0603n:101": "Gīvamantopavesayaṃ;",
  "anya-e0603n:102": "Olokayaṃsapassittha,",
  "anya-e0603n:103": "Nippannaṃtaṃsamātaraṃ.",
  "anya-e0603n:104": "20.",
  "anya-e0603n:105": "Gaṇheyyaṃtaṃapanetvā,",
  "anya-e0603n:106": "Bāhuṃcedeviyāahaṃ;",
  "anya-e0603n:107": "Nadadenikkhamokāsaṃ,",
  "anya-e0603n:108": "Pabujjhitvāyasodharā.",
  "anya-e0603n:109": "21.",
  "anya-e0603n:110": "Buddhapattovapasseyyaṃ,",
  "anya-e0603n:111": "Rāhulaṃiticintiya;",
  "anya-e0603n:112": "Kaṇḍakaṃāruhitvāna,",
  "anya-e0603n:113": "Tamhāchannenanikkhami.",
  "anya-e0603n:114": "22.",
  "anya-e0603n:115": "Dvārāsayāvivariṃsu,",
  "anya-e0603n:116": "Devatādvāramaṇḍalaṃ;",
  "anya-e0603n:117": "Ratanukkāsahassāni,",
  "anya-e0603n:118": "Jālayiṃsumarūtahiṃ.",
  "anya-e0603n:119": "23.",
  "anya-e0603n:120": "Mānikkhamassusiddhattha,",
  "anya-e0603n:121": "Itotvaṃsattamedine;",
  "anya-e0603n:122": "Bhavissasecakkavattī,",
  "anya-e0603n:123": "Itinīvāripāpimā.",
  "anya-e0603n:124": "24.",
  "anya-e0603n:125": "Tagghajānāmitaṃmāra,",
  "anya-e0603n:126": "Idhamātiṭṭhapakkama;",
  "anya-e0603n:127": "Itimāraṃ palāpetvā,",
  "anya-e0603n:128": "Vissaṭṭhosoabhikkami.",
  "anya-e0603n:129": "25.",
  "anya-e0603n:130": "Patvānomanadītīraṃ,",
  "anya-e0603n:131": "Punāgamesituragaṃ;",
  "anya-e0603n:132": "Channañcanagaraṃ bimbā;",
  "anya-e0603n:133": "Devilāpaṭivedituṃ.",
  "anya-e0603n:134": "26.",
  "anya-e0603n:135": "Bandhitaṃyasodharāya,",
  "anya-e0603n:136": "Moḷiṃsugandhavāsitaṃ;",
  "anya-e0603n:137": "Chetvākhaggenatiṇhena,",
  "anya-e0603n:138": "Ukkhipitthatamambare.",
  "anya-e0603n:139": "27.",
  "anya-e0603n:140": "Aṭṭhaseṭṭhaparikkhāre,",
  "anya-e0603n:141": "Dhāretvābrahmunābhate;",
  "anya-e0603n:142": "Hitvārājārahaṃdussaṃ,",
  "anya-e0603n:143": "Samaṇatthamupāgami.",
  "anya-e0603n:144": "28.",
  "anya-e0603n:145": "Karontodukkaraṃkammaṃ,",
  "anya-e0603n:146": "Chabbassāniparakkamo;",
  "anya-e0603n:147": "Oḷārikannapānehi;",
  "anya-e0603n:148": "Dehaṃyāpesiṭhītiyā.",
  "anya-e0603n:149": "29.",
  "anya-e0603n:150": "Visākhapuṇṇamehema,",
  "anya-e0603n:151": "Pātiṃpāyasapūritaṃ;",
  "anya-e0603n:152": "Sujātāyābhataṃgayha,",
  "anya-e0603n:153": "Nerañcaramupāgami.",
  "anya-e0603n:154": "30.",
  "anya-e0603n:155": "Nerañcarāyatīramhi,",
  "anya-e0603n:156": "Sutvāpāyāsamuttamaṃ;",
  "anya-e0603n:157": "Paṭiyatthavaramaggena,",
  "anya-e0603n:158": "Bodhimaṇḍamupāgami.",
  "anya-e0603n:159": "31.",
  "anya-e0603n:160": "Jayāsanepallaṅkamhi,",
  "anya-e0603n:161": "Ṭhitodhiṭṭhesidhītimā;",
  "anya-e0603n:162": "Navīriyaṃjahissāmi,",
  "anya-e0603n:163": "Apattoitibuddhataṃ.",
  "anya-e0603n:164": "32.",
  "anya-e0603n:165": "Devindodhamayaṃsaṅkhaṃ,",
  "anya-e0603n:166": "Setacchattaṃpitāmaho;",
  "anya-e0603n:167": "Dhārayaṃvādentovīṇaṃ,",
  "anya-e0603n:168": "Pañcasīkhoṭhitotahiṃ.",
  "anya-e0603n:169": "33.",
  "anya-e0603n:170": "Thutigītānigāyantī,",
  "anya-e0603n:171": "Aṭṭhāsuṃdevasundarī;",
  "anya-e0603n:172": "Gahetvāhemamañjūsā,",
  "anya-e0603n:173": "Surapupphehipūritā.",
  "anya-e0603n:174": "34.",
  "anya-e0603n:175": "Anatthamentesūriye,",
  "anya-e0603n:176": "Mārasenaṃpalāpayī;",
  "anya-e0603n:177": "Aruṇuggamanekāle,",
  "anya-e0603n:178": "Pattosibuddhataṃvaraṃ.",
  "anya-e0603n:179": "35.",
  "anya-e0603n:180": "Paṭṭhānaṃsammasantassa,",
  "anya-e0603n:181": "Nāthassadehatosubhā;",
  "anya-e0603n:182": "Chabbaṇṇabhānicchadhanti,",
  "anya-e0603n:183": "Vidhāvanti tahiṃ tahiṃ.",
  "anya-e0603n:184": "36.",
  "anya-e0603n:185": "Nīlānīlāpitāpītā,",
  "anya-e0603n:186": "Rattārattācanicchare;",
  "anya-e0603n:187": "Setāsetācamañciṭṭhā,",
  "anya-e0603n:188": "Tamhātamhāpabhassarā.",
  "anya-e0603n:189": "37.",
  "anya-e0603n:190": "Buddhapattopibodheyya,",
  "anya-e0603n:191": "Dhammagambhīrataṃjino;",
  "anya-e0603n:192": "Nisāmetvānadesetuṃ,",
  "anya-e0603n:193": "Nirussāhamupāgato.",
  "anya-e0603n:194": "38.",
  "anya-e0603n:195": "Sahampatitadābrahmā,",
  "anya-e0603n:196": "Desanāyābhiyācati;",
  "anya-e0603n:197": "Tenasopathamaṃseṭṭhaṃ,",
  "anya-e0603n:198": "Dhammacakkamadesayi.",
  "anya-e0603n:199": "39.",
  "anya-e0603n:200": "Ekavassovasambuddho,",
  "anya-e0603n:201": "Varephagguṇapuṇṇame;",
  "anya-e0603n:202": "Sakyānaṃnagaraṃgañchī,",
  "anya-e0603n:203": "Sāvakehipurakkhato.",
  "anya-e0603n:204": "40.",
  "anya-e0603n:205": "Samāgamamhiñātīnaṃ,",
  "anya-e0603n:206": "Vassāpesisusītalaṃ;",
  "anya-e0603n:207": "Mahāmeghañcadesesi,",
  "anya-e0603n:208": "Vessantarassajātakaṃ.",
  "anya-e0603n:209": "41.",
  "anya-e0603n:210": "Gantvāsāvakayugena,",
  "anya-e0603n:211": "Yasodharāyamandiraṃ;",
  "anya-e0603n:212": "Taṃtosetvānadesesi,",
  "anya-e0603n:213": "Candakinnarajātakaṃ.",
  "anya-e0603n:214": "42.",
  "anya-e0603n:215": "Vasitvāsattamevasse,",
  "anya-e0603n:216": "Tāvatiṃse silāsane;",
  "anya-e0603n:217": "Abhidhammena tosesi,",
  "anya-e0603n:218": "Devabrahmagaṇaṃbahuṃ.",
  "anya-e0603n:219": "43.",
  "anya-e0603n:220": "Pattharaṃñāṇajālena,",
  "anya-e0603n:221": "Lokaṃlokesisabbadā;",
  "anya-e0603n:222": "Bodhetuṃbodhiyābhāya;",
  "anya-e0603n:223": "Bodheyyajanapaṅkajaṃ.",
  "anya-e0603n:224": "44.",
  "anya-e0603n:225": "Bodhaneyyaṃjanaṃdisvā,",
  "anya-e0603n:226": "Cakkavāḷaparaṃparā;",
  "anya-e0603n:227": "Gantvāpivibodhesi,",
  "anya-e0603n:228": "Buddhaseṭṭhosubodhimā.",
  "anya-e0603n:229": "45.",
  "anya-e0603n:230": "Bakādikecabrahmāno,",
  "anya-e0603n:231": "Yakkhecāḷavakādike;",
  "anya-e0603n:232": "Purindadādikedeve,",
  "anya-e0603n:233": "Sūcilomādirakkhase.",
  "anya-e0603n:234": "46.",
  "anya-e0603n:235": "Jāṇusoṇādikeneke,",
  "anya-e0603n:236": "Brāhmaṇecāvikakkhaḷe;",
  "anya-e0603n:237": "Cūḷodarādikenāge,",
  "anya-e0603n:238": "Dhanapālādikegaje.",
  "anya-e0603n:239": "47.",
  "anya-e0603n:240": "Buddhāmayaṃtimaññante,",
  "anya-e0603n:241": "Titthiyenekaladdhike;",
  "anya-e0603n:242": "Damesidamataṃseṭṭho,",
  "anya-e0603n:243": "Paravādappamaddano.",
  "anya-e0603n:244": "48.",
  "anya-e0603n:245": "Anubyañjanasampanno,",
  "anya-e0603n:246": "Esabāttiṃsalakkhaṇo;",
  "anya-e0603n:247": "Byāmappabhāparikkhitto,",
  "anya-e0603n:248": "Ketumālāsilaṅkhato.",
  "anya-e0603n:249": "49.",
  "anya-e0603n:250": "Caturāsītisahassa,",
  "anya-e0603n:251": "Dhammakkhandhesadesayi;",
  "anya-e0603n:252": "Pañcatāthīsavassāni,",
  "anya-e0603n:253": "Tilokassahitāyave.",
  "anya-e0603n:254": "50.",
  "anya-e0603n:255": "Buddhakiccaṃkarontoso,",
  "anya-e0603n:256": "Vahaṃlokahitaṃvaraṃ;",
  "anya-e0603n:257": "Mocayaṃbandhanāsatte,",
  "anya-e0603n:258": "Kālaṃkhepetisabbadā.",
  "anya-e0603n:259": "51.",
  "anya-e0603n:260": "Yahiṃ āsītikonātho,",
  "anya-e0603n:261": "Nibbutovaramañjake;",
  "anya-e0603n:262": "Mahāsālavaneramme,",
  "anya-e0603n:263": "Mallānaṃnigametahiṃ.",
  "anya-e0603n:264": "52.",
  "anya-e0603n:265": "Aggikkhandhovalokasmiṃ,",
  "anya-e0603n:266": "Jāletvānasasāvako;",
  "anya-e0603n:267": "Nibbāyitthasaṅkhatānaṃ,",
  "anya-e0603n:268": "Pakāsentoaniccataṃti.",
  "anya-e0603n:269": "‘‘Namotebhagavānātha,",
  "anya-e0603n:270": "Namotejayataṃjaya;",
  "anya-e0603n:271": "Namotesirisampanna,",
  "anya-e0603n:272": "Namotelokanandana.’’",
  "anya-e0603n:273": "Buddhavandanā",
  "anya-e0603n:274": "1.",
  "anya-e0603n:275": "Devalokā cavitvāna,",
  "anya-e0603n:276": "Mahāmāyāya kucchiyaṃ;",
  "anya-e0603n:277": "Uppajji guruvāramhi,",
  "anya-e0603n:278": "Vandetaṃ sakyapuṅgavaṃ.",
  "anya-e0603n:279": "2.",
  "anya-e0603n:280": "Dasamā saccaye neso,",
  "anya-e0603n:281": "Vijāyi mātukucchito;",
  "anya-e0603n:282": "Sukkavāre lumbiniyaṃ,",
  "anya-e0603n:283": "Vande taṃ lokapūjitaṃ.",
  "anya-e0603n:284": "3.",
  "anya-e0603n:285": "Cakkavattisiriṃ hitvā,",
  "anya-e0603n:286": "Mahāsinikkhamaṃ sudhī;",
  "anya-e0603n:287": "Nikkhamī candavāramhi,",
  "anya-e0603n:288": "Name taṃ munikuñjaraṃ.",
  "anya-e0603n:289": "4.",
  "anya-e0603n:290": "Assatthabodhimūlamhi,",
  "anya-e0603n:291": "Pallaṅke aparājite;",
  "anya-e0603n:292": "Patto sabbaññutaṃ nātho,",
  "anya-e0603n:293": "Name taṃ budhavāsare.",
  "anya-e0603n:294": "5.",
  "anya-e0603n:295": "Pañcavaggiyamukhānaṃ,",
  "anya-e0603n:296": "Devānaṃ migadāvane;",
  "anya-e0603n:297": "Sorivāre dhammacakkaṃ,",
  "anya-e0603n:298": "Vattesi taṃ jinaṃ name.",
  "anya-e0603n:299": "6.",
  "anya-e0603n:300": "Sabbasaṅkhatadhammānaṃ,",
  "anya-e0603n:301": "Pakāsiyaaniccataṃ;",
  "anya-e0603n:302": "Nibbutaṃaṅgavāramhi,",
  "anya-e0603n:303": "Namādhiamatandadaṃ.",
  "anya-e0603n:304": "7.",
  "anya-e0603n:305": "Sūriyavāre soṇṇāya,",
  "anya-e0603n:306": "Doṇiyā jinaviggaho;",
  "anya-e0603n:307": "Agginujjalito tassa,",
  "anya-e0603n:308": "Name dhātusarīrakaṃ.",
  "anya-e0603n:309": "Uṇṇālomikanātha vandanā",
  "anya-e0603n:310": "Uṇṇālodhikanāthassa,",
  "anya-e0603n:311": "Uṇṇāya bhamukantare;",
  "anya-e0603n:312": "Vajirāviya sobhanti,",
  "anya-e0603n:313": "Nikkhanti yo supaṇḍarā.",
  "anya-e0603n:314": "Vajiragghanakāyassa,",
  "anya-e0603n:315": "Nāthassa dehato subhā;",
  "anya-e0603n:316": "Vajireyyā niccharanti;",
  "anya-e0603n:317": "Vijjūva gagaṇantare.",
  "anya-e0603n:318": "Rinindamandahāsena,",
  "anya-e0603n:319": "Dāṭhāya niccharā pabhā;",
  "anya-e0603n:320": "Vipphūrisā disāsabbā,",
  "anya-e0603n:321": "Vajirāyanti paṇḍarā."
}