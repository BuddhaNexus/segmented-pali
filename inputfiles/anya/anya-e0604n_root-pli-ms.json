{
  "anya-e0604n:6": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e0604n:7": "Suttavandanā",
  "anya-e0604n:8": "1.",
  "anya-e0604n:9": "Dhammattha desanā vedha,",
  "anya-e0604n:10": "Catugambhīra duddasaṃ;",
  "anya-e0604n:11": "Dhammaṃ lokassa desentaṃ,",
  "anya-e0604n:12": "Dhammarājaṃ namāmahaṃ;",
  "anya-e0604n:13": "2.",
  "anya-e0604n:14": "Buddho paccekabuddho ca,",
  "anya-e0604n:15": "Arahā cakkavattīti;",
  "anya-e0604n:16": "Thūpāthūpārahaṃ vande,",
  "anya-e0604n:17": "Catuthūpārahesu taṃ;",
  "anya-e0604n:18": "3.",
  "anya-e0604n:19": "Petaseyyo kāmabhogī,",
  "anya-e0604n:20": "Sīhaseyyo tathāgato;",
  "anya-e0604n:21": "Sayantaṃ catuseyyesu,",
  "anya-e0604n:22": "Catutthena name jinaṃ;",
  "anya-e0604n:23": "4.",
  "anya-e0604n:24": "Aṇḍajā jalābujā ca;",
  "anya-e0604n:25": "Saṃsedajo papātikā;",
  "anya-e0604n:26": "Catuyoni pariccheda,",
  "anya-e0604n:27": "Ñāṇavantaṃ name jinaṃ;",
  "anya-e0604n:28": "5.",
  "anya-e0604n:29": "Sutabuddhonubuddho ca,",
  "anya-e0604n:30": "Pacceko jinabuddhoti;",
  "anya-e0604n:31": "Catutthaṃ catubuddhesu,",
  "anya-e0604n:32": "Vandāmi lokanāyakaṃ;",
  "anya-e0604n:33": "6.",
  "anya-e0604n:34": "Rāhuggaṃ attabhāgīnaṃ,",
  "anya-e0604n:35": "Mandhātā kāmabhogīnaṃ;",
  "anya-e0604n:36": "Māro adhipateyyānaṃ,",
  "anya-e0604n:37": "Buddho loke sadevake;",
  "anya-e0604n:38": "Catuagga paññattīnaṃ,",
  "anya-e0604n:39": "Aggapattaṃ name jinaṃ.",
  "anya-e0604n:40": "7.",
  "anya-e0604n:41": "Dānaṃ piyakathā attha,",
  "anya-e0604n:42": "Cariyā sadisattatā;",
  "anya-e0604n:43": "Catusaṅgaha vatthūhi,",
  "anya-e0604n:44": "Asaṅkhiyesu jātisu;",
  "anya-e0604n:45": "Lokassa saṅgahetāraṃ,",
  "anya-e0604n:46": "Vandāmi lokanāyakaṃ;",
  "anya-e0604n:47": "8.",
  "anya-e0604n:48": "Lobho doso ca mohoti,",
  "anya-e0604n:49": "Anto paccatthike tayo;",
  "anya-e0604n:50": "Dujjaye sabbaverīhi,",
  "anya-e0604n:51": "Sañjitaṃpi sudhiṃ name;",
  "anya-e0604n:52": "9.",
  "anya-e0604n:53": "Jāti puñña mahattāni,",
  "anya-e0604n:54": "Guṇamahattamuttamaṃ;",
  "anya-e0604n:55": "Timahatthehi sampannaṃ,",
  "anya-e0604n:56": "Vandāmi lokanāyakaṃ;",
  "anya-e0604n:57": "10.",
  "anya-e0604n:58": "Hetu phala sampadāyo,",
  "anya-e0604n:59": "Sattupakārasampadā;",
  "anya-e0604n:60": "Tisampadāhi sampannaṃ,",
  "anya-e0604n:61": "Vandāmi lokanāyakaṃ.",
  "anya-e0604n:62": "11.",
  "anya-e0604n:63": "Ñāṇappahānānubhāva,",
  "anya-e0604n:64": "Surūpakārasampadā;",
  "anya-e0604n:65": "Catuphala sampadāhi,",
  "anya-e0604n:66": "Taṃ sampannaṃ name jinaṃ.",
  "anya-e0604n:67": "12.",
  "anya-e0604n:68": "Āsayo ca payogoti,",
  "anya-e0604n:69": "Sattupakāra sampadā;",
  "anya-e0604n:70": "Duvidhā yassa sampannā,",
  "anya-e0604n:71": "Sammā maṃ pātu so jino.",
  "anya-e0604n:72": "13.",
  "anya-e0604n:73": "Mahosadhe vessantare,",
  "anya-e0604n:74": "Pacchimabhavike tathā;",
  "anya-e0604n:75": "Tijātīsu name vācaṃ,",
  "anya-e0604n:76": "Bhāsentaṃ jāyanakkhaṇe.",
  "anya-e0604n:77": "14.",
  "anya-e0604n:78": "Nemimhi sādhine ceva,",
  "anya-e0604n:79": "Mandhātari ca guttile;",
  "anya-e0604n:80": "Catūsu manujattena,",
  "anya-e0604n:81": "Devalokaṃ gataṃ name.",
  "anya-e0604n:82": "15.",
  "anya-e0604n:83": "Saṅkhāro rūpavikāro,",
  "anya-e0604n:84": "Lakkhaṇaṃ vānanissaṭaṃ;",
  "anya-e0604n:85": "Nibbānaṃ paññatticeti,",
  "anya-e0604n:86": "Pañca ñeyyantaguṃ name.",
  "anya-e0604n:87": "16.",
  "anya-e0604n:88": "Putta dāra pariccāgā,",
  "anya-e0604n:89": "Rajjaṅgaṃ jīva cāgīti;",
  "anya-e0604n:90": "Pañca mahāpariccāge,",
  "anya-e0604n:91": "Karaṃ sudukkare name.",
  "anya-e0604n:92": "17.",
  "anya-e0604n:93": "Rūpañca vedanā saññā,",
  "anya-e0604n:94": "Saṅkhārā viññāṇantime;",
  "anya-e0604n:95": "Pañcakkhandhe parijānaṃ,",
  "anya-e0604n:96": "Tipariññāhi taṃ name.",
  "anya-e0604n:97": "18.",
  "anya-e0604n:98": "Manussa deva gatiyo,",
  "anya-e0604n:99": "Peto ca nirayo tathā;",
  "anya-e0604n:100": "Tiracchānoti pañcetā,",
  "anya-e0604n:101": "Chindantaṃ gatiyo name;",
  "anya-e0604n:102": "19.",
  "anya-e0604n:103": "Devaputto kileso ca,",
  "anya-e0604n:104": "Abhisaṅkhāra mārako;",
  "anya-e0604n:105": "Khandho maccūti pañcete,",
  "anya-e0604n:106": "Māre vijitavaṃ name.",
  "anya-e0604n:107": "20.",
  "anya-e0604n:108": "Purebhattaṃ pacchābhattaṃ,",
  "anya-e0604n:109": "Purimañceva majjhimaṃ;",
  "anya-e0604n:110": "Pacchimanti sadā pañca,",
  "anya-e0604n:111": "Buddhakiccaṃ karaṃ name.",
  "anya-e0604n:112": "21.",
  "anya-e0604n:113": "Rūpā saddā gandhā rasā,",
  "anya-e0604n:114": "Phoṭṭhabbā ca manoramā;",
  "anya-e0604n:115": "Pañca kāmaguṇe hitvā,",
  "anya-e0604n:116": "Kheḷaṃva niggataṃ name.",
  "anya-e0604n:117": "22.",
  "anya-e0604n:118": "Rāgasallaṃ dosasallaṃ,",
  "anya-e0604n:119": "Mohā māno ca diṭṭhīti;",
  "anya-e0604n:120": "Vande vajjātivajjantaṃ,",
  "anya-e0604n:121": "Pañcasallapanūdanaṃ.",
  "anya-e0604n:122": "23.",
  "anya-e0604n:123": "Dīgho majjhima saṃyuttā,",
  "anya-e0604n:124": "Aṅguttaro ca khuddako;",
  "anya-e0604n:125": "Iti pañca nikāyehi,",
  "anya-e0604n:126": "Suvinentaṃ pajaṃ name.",
  "anya-e0604n:127": "24.",
  "anya-e0604n:128": "Issariyaṃ yaso dhammo,",
  "anya-e0604n:129": "Kāmo sirī payattīti;",
  "anya-e0604n:130": "Bhagyehi chahi sampannaṃ,",
  "anya-e0604n:131": "Vandāmi lokanāyakaṃ.",
  "anya-e0604n:132": "25.",
  "anya-e0604n:133": "Piyāpiyaṃ bhūtābhūtaṃ,",
  "anya-e0604n:134": "Atthānatthaṃ yathārahaṃ;",
  "anya-e0604n:135": "Missā chasu suvācaṃva,",
  "anya-e0604n:136": "Bhāsantaṃ atthakaṃ name.",
  "anya-e0604n:137": "26.",
  "anya-e0604n:138": "Alaso ca pamādo ca,",
  "anya-e0604n:139": "Anuṭṭhānaṃ asaṃyamo;",
  "anya-e0604n:140": "Niddā tantīti chiddehi,",
  "anya-e0604n:141": "Chahi muttaṃ sadā name.",
  "anya-e0604n:142": "27.",
  "anya-e0604n:143": "Mahākaruṇāsamāpatti,",
  "anya-e0604n:144": "Yamakappāṭihāriyaṃ;",
  "anya-e0604n:145": "Āsayānusaye ñāṇaṃ,",
  "anya-e0604n:146": "Indriyāna paropare;",
  "anya-e0604n:147": "Sabbaññutā nāvaraṇaṃ,",
  "anya-e0604n:148": "Chā sādhāraṇikaṃ name.",
  "anya-e0604n:149": "28.",
  "anya-e0604n:150": "Kāmacchando ca byāpādo,",
  "anya-e0604n:151": "Thinamiddhañca saṃsayo;",
  "anya-e0604n:152": "Avijjuddhacca kukkuccaṃ,",
  "anya-e0604n:153": "Chavinīvaraṇaṃ name.",
  "anya-e0604n:154": "29.",
  "anya-e0604n:155": "Iddhividhaṃ dibbasotaṃ,",
  "anya-e0604n:156": "Paracitta vijānanā;",
  "anya-e0604n:157": "Pubbenivāsānussati,",
  "anya-e0604n:158": "Dibbacakkhāsavakkhayo;",
  "anya-e0604n:159": "Chaḷabhiññāhi sampannaṃ,",
  "anya-e0604n:160": "Vandāmi purisuttamaṃ.",
  "anya-e0604n:161": "30.",
  "anya-e0604n:162": "Nīla pītā ca odātā,",
  "anya-e0604n:163": "Mañjiṭṭhā ca pabhassarā;",
  "anya-e0604n:164": "Lohitāti cha raṃsīhī,",
  "anya-e0604n:165": "Vijjotantaṃ sadā name.",
  "anya-e0604n:166": "31.",
  "anya-e0604n:167": "Assādādīnavo ceva,",
  "anya-e0604n:168": "Nissaraṇaphalaṃ tathā;",
  "anya-e0604n:169": "Upāyāṇattīti chadhā,",
  "anya-e0604n:170": "Saddhammadesakaṃ name.",
  "anya-e0604n:171": "32.",
  "anya-e0604n:172": "Rāgo doso ca moho ca,",
  "anya-e0604n:173": "Vitakkacariyā tathā;",
  "anya-e0604n:174": "Saddhā buddhīti bhājentaṃ,",
  "anya-e0604n:175": "Tā chaḷecariyā name.",
  "anya-e0604n:176": "33.",
  "anya-e0604n:177": "Rajataṃ kanakaṃ muttā,",
  "anya-e0604n:178": "Maṇi veḷuriyāni ca;",
  "anya-e0604n:179": "Vajirañca pavāḷanti,",
  "anya-e0604n:180": "Seṭṭhi sattadhano viya.",
  "anya-e0604n:181": "34.",
  "anya-e0604n:182": "Saddhāsīlaṃ sutaṃcāgo,",
  "anya-e0604n:183": "Hirī ottappiyaṃ dhanaṃ,",
  "anya-e0604n:184": "Paññādhananti sambuddhaṃ,",
  "anya-e0604n:185": "Vande sattamahādhanaṃ.",
  "anya-e0604n:186": "35.",
  "anya-e0604n:187": "Cakkaṃ hatthi asso maṇi,",
  "anya-e0604n:188": "Gahapatitthiyo tathā;",
  "anya-e0604n:189": "Supariṇāyakocāti,",
  "anya-e0604n:190": "Sattaratano cakkavattīva.",
  "anya-e0604n:191": "36.",
  "anya-e0604n:192": "Sati dhammavicayo ca,",
  "anya-e0604n:193": "Tatheva vīriyaṃ pīti;",
  "anya-e0604n:194": "Passaddhi samādhupekkhā,",
  "anya-e0604n:195": "Saddhamma cakkavattikaṃ;",
  "anya-e0604n:196": "Satta bojjhaṅgaratanaṃ,",
  "anya-e0604n:197": "Namāmi purisuttamaṃ.",
  "anya-e0604n:198": "37.",
  "anya-e0604n:199": "Yugandharo īsadharo,",
  "anya-e0604n:200": "Karavīko sudassano;",
  "anya-e0604n:201": "Nemindharo vinatako,",
  "anya-e0604n:202": "Assakaṇṇo mahānage;",
  "anya-e0604n:203": "Sattete hattharūpaṃva,",
  "anya-e0604n:204": "Sampassantaṃ jinaṃ name.",
  "anya-e0604n:205": "38.",
  "anya-e0604n:206": "Kaṇṇamuṇḍo anotatto,",
  "anya-e0604n:207": "Kuṇālo rathakārako;",
  "anya-e0604n:208": "Sīhappapāta chaddantā,",
  "anya-e0604n:209": "Mandākinī mahāsare;",
  "anya-e0604n:210": "Sattete sabbathā passaṃ,",
  "anya-e0604n:211": "Diṭṭheva pātikaṃ name.",
  "anya-e0604n:212": "39.",
  "anya-e0604n:213": "Manussattaṃ liṅgasampatti,",
  "anya-e0604n:214": "Hetu satthāradassanaṃ;",
  "anya-e0604n:215": "Pabbajjā guṇasampatti,",
  "anya-e0604n:216": "Adhikāro ca chandatā;",
  "anya-e0604n:217": "Aṭṭhadhamma samodhānā,",
  "anya-e0604n:218": "Bhinīhārantaguṃ name.",
  "anya-e0604n:219": "40.",
  "anya-e0604n:220": "Visaṭṭhaṃ mañju viññeyyaṃ,",
  "anya-e0604n:221": "Savanīyāvisāriṇā;",
  "anya-e0604n:222": "Gambhīro binduninnādo,",
  "anya-e0604n:223": "Aṭṭhaṅgikasaraṃ name.",
  "anya-e0604n:224": "41.",
  "anya-e0604n:225": "Cattāri rūpajhānāni,",
  "anya-e0604n:226": "Tathevārūpajhānāni;",
  "anya-e0604n:227": "Tā samāpattiyo aṭṭha,",
  "anya-e0604n:228": "Sevantaṃ ruciyā name.",
  "anya-e0604n:229": "42.",
  "anya-e0604n:230": "Aṇimā laṅghimā kammaṃ,",
  "anya-e0604n:231": "Mahimā pattimā tathā;",
  "anya-e0604n:232": "Īsitā vasitā ceva,",
  "anya-e0604n:233": "Yatthakāmāvasāyitā;",
  "anya-e0604n:234": "Aṭṭhissariya puṇṇattā,",
  "anya-e0604n:235": "Issarātissaraṃ name.",
  "anya-e0604n:236": "43.",
  "anya-e0604n:237": "Manomayiddhiñāṇañca,",
  "anya-e0604n:238": "Chaḷabhiññā vipassanā;",
  "anya-e0604n:239": "Aṭṭhavijjāhi sampannaṃ,",
  "anya-e0604n:240": "Vande tilokaketukaṃ.",
  "anya-e0604n:241": "44.",
  "anya-e0604n:242": "Jāti jarā rujā kālo,",
  "anya-e0604n:243": "Caturāpāyadukkhāti;",
  "anya-e0604n:244": "Aṭṭha saṃvegavatthūni,",
  "anya-e0604n:245": "Bhāsantaṃ vividhaṃ name.",
  "anya-e0604n:246": "45.",
  "anya-e0604n:247": "Suttaṅgaṃ veyyākaraṇaṃ,",
  "anya-e0604n:248": "Geyyaṃ gāthā ca jātakaṃ;",
  "anya-e0604n:249": "Abbhūtadhamma vedallā,",
  "anya-e0604n:250": "Udānamiti vuttakaṃ;",
  "anya-e0604n:251": "Navaṅgaṃ sabbalokassa,",
  "anya-e0604n:252": "Dadantaṃ sāsanaṃ name.",
  "anya-e0604n:253": "46.",
  "anya-e0604n:254": "Yā samāpattiyo aṭṭha,",
  "anya-e0604n:255": "Nirodho cāti te nava,",
  "anya-e0604n:256": "Anupubbavihāre taṃ,",
  "anya-e0604n:257": "Sevantaṃ ruciyā name.",
  "anya-e0604n:258": "47.",
  "anya-e0604n:259": "Ekattañceva nānattaṃ,",
  "anya-e0604n:260": "Kāyasaññīhi yogato,",
  "anya-e0604n:261": "Cattāro tathāruppā ca,",
  "anya-e0604n:262": "Asaññīcāti te nava,",
  "anya-e0604n:263": "Sattavāse vibhajitvā,",
  "anya-e0604n:264": "Pakāsentaṃ name jinaṃ.",
  "anya-e0604n:265": "48.",
  "anya-e0604n:266": "Seyyassa sadiso seyyo,",
  "anya-e0604n:267": "Hīnoti tividho vidho,",
  "anya-e0604n:268": "Tathā sadisahīnānaṃ,",
  "anya-e0604n:269": "Navavidhe nudaṃ name.",
  "anya-e0604n:270": "49.",
  "anya-e0604n:271": "Anatthaṃ me carati ca,",
  "anya-e0604n:272": "Tathā cari carissati,",
  "anya-e0604n:273": "Mittassa arinotvatthaṃ,",
  "anya-e0604n:274": "Navāghāte jahaṃ name.",
  "anya-e0604n:275": "50.",
  "anya-e0604n:276": "Dānaṃ sīlañca nikkhamaṃ,",
  "anya-e0604n:277": "Paññā vīriyapañcamaṃ,",
  "anya-e0604n:278": "Khantī saccamadhiṭṭhānaṃ,",
  "anya-e0604n:279": "Mettupekkhāti tādasa,",
  "anya-e0604n:280": "Pūretvā pāramī vande,",
  "anya-e0604n:281": "Sammāsambodhimajjhavaṃ;",
  "anya-e0604n:282": "51.",
  "anya-e0604n:283": "Kāḷāvakañca gaṅgeyyaṃ,",
  "anya-e0604n:284": "Paṇḍaraṃ tamba piṅgalaṃ,",
  "anya-e0604n:285": "Gandha maṅgala hemañca,",
  "anya-e0604n:286": "Uposatha chaddantime.",
  "anya-e0604n:287": "52.",
  "anya-e0604n:288": "Dhāreti dasaposānaṃ,",
  "anya-e0604n:289": "Nāgo kāḷāvako balaṃ;",
  "anya-e0604n:290": "Sahassakoṭiposānaṃ;",
  "anya-e0604n:291": "Chaddantovāranuttamo.",
  "anya-e0604n:292": "53.",
  "anya-e0604n:293": "Dasa sahassakoṭīnaṃ,",
  "anya-e0604n:294": "Posānaṃ baladhāraṇaṃ;",
  "anya-e0604n:295": "Chaddantānaṃ dasannañca,",
  "anya-e0604n:296": "Dasakāyabalaṃ name.",
  "anya-e0604n:297": "54.",
  "anya-e0604n:298": "Ṭhānāṭhāne vipāke ca,",
  "anya-e0604n:299": "Magge sabbatthagāminaṃ;",
  "anya-e0604n:300": "Nānādhātūsu lokesu,",
  "anya-e0604n:301": "Adhimuttimhi pāṇinaṃ;",
  "anya-e0604n:302": "55.",
  "anya-e0604n:303": "Paropariyatte ñāṇaṃ,",
  "anya-e0604n:304": "Indriyānañca pāṇinaṃ;",
  "anya-e0604n:305": "Jhānādīsu ñāṇaṃ pubbe,",
  "anya-e0604n:306": "Nivāse dibbacakkhu ca;",
  "anya-e0604n:307": "Āsavakkhayañāṇanti,",
  "anya-e0604n:308": "Dasabalañāṇaṃ name.",
  "anya-e0604n:309": "56.",
  "anya-e0604n:310": "Mūlagandho sāro pheggu,",
  "anya-e0604n:311": "Taco papaṭiko raso;",
  "anya-e0604n:312": "Pattaṃ pupphaṃ phalañceva,",
  "anya-e0604n:313": "Gandhagandhoti saṃvaraṃ;",
  "anya-e0604n:314": "Sugandhaṃ dasagandhehi,",
  "anya-e0604n:315": "Vilimpantaṃ sadā name.",
  "anya-e0604n:316": "57.",
  "anya-e0604n:317": "Samādhi ñāṇavipphārā,",
  "anya-e0604n:318": "Adhiṭṭhānaṃ vikubbanā;",
  "anya-e0604n:319": "Manomayāriyā iddhi,",
  "anya-e0604n:320": "Tathā kammavipākajā.",
  "anya-e0604n:321": "58.",
  "anya-e0604n:322": "Vijjāmayā puññavato;",
  "anya-e0604n:323": "Payoga paccayiddhiti;",
  "anya-e0604n:324": "Aggapatta dasiddhīnaṃ,",
  "anya-e0604n:325": "Pāṭiheraṃ karaṃ name.",
  "anya-e0604n:326": "59.",
  "anya-e0604n:327": "Muttā maṇi veḷuriyaṃ,",
  "anya-e0604n:328": "Saṅkho sallo pavāḷakaṃ;",
  "anya-e0604n:329": "Suvaṇṇaṃ rajataṃ lohi,",
  "anya-e0604n:330": "Taṅko masāragallanti;",
  "anya-e0604n:331": "Dasadhā ratanehiggaṃ,",
  "anya-e0604n:332": "Bhogiṃ paññāmaṇiṃ name.",
  "anya-e0604n:333": "60.",
  "anya-e0604n:334": "Sithilaṃ dhanitaṃ dīghaṃ,",
  "anya-e0604n:335": "Rassañca garukaṃ lahu;",
  "anya-e0604n:336": "Niggahitaṃ vimuttañca,",
  "anya-e0604n:337": "Sambandhañca vavatthitaṃ;",
  "anya-e0604n:338": "Vivarantaṃ name dhammaṃ,",
  "anya-e0604n:339": "Dasabyañjanabheditaṃ.",
  "anya-e0604n:340": "61.",
  "anya-e0604n:341": "Pasādojo madhuratā,",
  "anya-e0604n:342": "Samatā sukhumālatā;",
  "anya-e0604n:343": "Silesodāratā kanti,",
  "anya-e0604n:344": "Atthabyatti samādhayo;",
  "anya-e0604n:345": "Dasa saddaguṇopetaṃ,",
  "anya-e0604n:346": "Dhammaṃ pātu karaṃ name.",
  "anya-e0604n:347": "62.",
  "anya-e0604n:348": "Lobho doso moho māno,",
  "anya-e0604n:349": "Uddhaccaṃ diṭṭhi saṃsayo;",
  "anya-e0604n:350": "Ahirikamanottappaṃ,",
  "anya-e0604n:351": "Thinanti sabba dāhake;",
  "anya-e0604n:352": "Kilese dasa te saddhiṃ,",
  "anya-e0604n:353": "Vāsanāya jahaṃ name.",
  "anya-e0604n:354": "63.",
  "anya-e0604n:355": "Saterā daṇḍamaṇikā,",
  "anya-e0604n:356": "Macchavilolagaggarā;",
  "anya-e0604n:357": "Sukkhāsani kapisisā,",
  "anya-e0604n:358": "Vicakkā sañña kukkuṭā;",
  "anya-e0604n:359": "Navāsanīhi phālentaṃ,",
  "anya-e0604n:360": "Tibbaṃ paññāsaniṃ name.",
  "anya-e0604n:361": "64.",
  "anya-e0604n:362": "Rodanā kodhāvudho ca,",
  "anya-e0604n:363": "Ujjhantissariyaṃ tathā;",
  "anya-e0604n:364": "Paṭisaṅkhānaṃ,",
  "anya-e0604n:365": "Khanti aṭṭhabalesu taṃ;",
  "anya-e0604n:366": "Pathavīsadisaṃ vande,",
  "anya-e0604n:367": "Aṭṭhamena vināyakaṃ.",
  "anya-e0604n:368": "65.",
  "anya-e0604n:369": "Kāmarogo ca paṭigho,",
  "anya-e0604n:370": "Māno diṭṭhi ca saṃsayo;",
  "anya-e0604n:371": "Bhavarāgo avijjāti,",
  "anya-e0604n:372": "Sattānusayinaṃ name.",
  "anya-e0604n:373": "66.",
  "anya-e0604n:374": "Titthiyāpakatimahāsāvakā,",
  "anya-e0604n:375": "Aggasāvakā paccekabuddhasambuddhā;",
  "anya-e0604n:376": "Visiṭṭhaṃ sabbathā chasu,",
  "anya-e0604n:377": "Pubbe nivāsa viññūsu;",
  "anya-e0604n:378": "Tilokamakuṭaṃ name.",
  "anya-e0604n:379": "67.",
  "anya-e0604n:380": "Buddhacakkhu samantā ca,",
  "anya-e0604n:381": "Ñāṇaṃ yaṃ dibbacakkhu ca;",
  "anya-e0604n:382": "Dhammoti pañca cakkhūhi,",
  "anya-e0604n:383": "Dassāviṃ mārajiṃ name.",
  "anya-e0604n:384": "68.",
  "anya-e0604n:385": "Khuddakā khaṇikā okkantikā ca,",
  "anya-e0604n:386": "Pharaṇā tathā ubbegā;",
  "anya-e0604n:387": "Pañca pītīhi pinentaṃ,",
  "anya-e0604n:388": "Ruciyā ca namāmahaṃ.",
  "anya-e0604n:389": "69.",
  "anya-e0604n:390": "Dukkhaṃ samudayasaccaṃ,",
  "anya-e0604n:391": "Nirodho maggasaccakaṃ;",
  "anya-e0604n:392": "Catusaccamabhijānaṃ,",
  "anya-e0604n:393": "Catukiccehi taṃ name.",
  "anya-e0604n:394": "70.",
  "anya-e0604n:395": "Pīḷanā saṅkhatattho ca,",
  "anya-e0604n:396": "Tapo vipariṇāmanā;",
  "anya-e0604n:397": "Dukkhasaccaṃ catukkehi,",
  "anya-e0604n:398": "Vibhattāviṃ muniṃ name.",
  "anya-e0604n:399": "71.",
  "anya-e0604n:400": "Āyūhanaṃ nidānañca,",
  "anya-e0604n:401": "Saṃyogo palibodhanaṃ;",
  "anya-e0604n:402": "Catukkehi samudayaṃ,",
  "anya-e0604n:403": "Vibhattāviṃ muniṃ name.",
  "anya-e0604n:404": "72.",
  "anya-e0604n:405": "Nissaraṇañca viveko,",
  "anya-e0604n:406": "Asaṅkhatāmataṃ tathā;",
  "anya-e0604n:407": "Nirodhañca catukkehi,",
  "anya-e0604n:408": "Vibhattāviṃ muniṃ name.",
  "anya-e0604n:409": "73.",
  "anya-e0604n:410": "Niyyāniko ca hetvattho,",
  "anya-e0604n:411": "Dassanādhipateyyakaṃ;",
  "anya-e0604n:412": "Maggasaccaṃ catukkehi,",
  "anya-e0604n:413": "Vibhattāviṃ muniṃ name.",
  "anya-e0604n:414": "74.",
  "anya-e0604n:415": "Satthako ca asammoho,",
  "anya-e0604n:416": "Sappāyo ceva gocaro;",
  "anya-e0604n:417": "Catusampajaññā vinā,",
  "anya-e0604n:418": "Bhāviṃ name tathāgataṃ.",
  "anya-e0604n:419": "75.",
  "anya-e0604n:420": "Pahinākhila dukkhāhaṃ,",
  "anya-e0604n:421": "Bhavasāgara pāraguṃ;",
  "anya-e0604n:422": "Vande sāraguṇopetaṃ,",
  "anya-e0604n:423": "Tenamhi bhavapārago.",
  "anya-e0604n:424": "76.",
  "anya-e0604n:425": "Rūpārūpavilāsagga,",
  "anya-e0604n:426": "Rūpācinteyya saṃyuttaṃ;",
  "anya-e0604n:427": "Vande sāraguṇopetaṃ,",
  "anya-e0604n:428": "Tenamhātularūpavā.",
  "anya-e0604n:429": "77.",
  "anya-e0604n:430": "Iddhi iddhi vilāsagga,",
  "anya-e0604n:431": "Iddhi cinteyya saṃyuttaṃ;",
  "anya-e0604n:432": "Vande sāraguṇepetaṃ,",
  "anya-e0604n:433": "Tenamhā tulaiddhimā.",
  "anya-e0604n:434": "78.",
  "anya-e0604n:435": "Vācā vācā vilāsagga,",
  "anya-e0604n:436": "Vācā cinteyya saṃyuttaṃ;",
  "anya-e0604n:437": "Vande sāraguṇopetaṃ,",
  "anya-e0604n:438": "Tenamhā tulavācako.",
  "anya-e0604n:439": "79.",
  "anya-e0604n:440": "Ñāṇa ñāṇa vilāsagga,",
  "anya-e0604n:441": "Ñāṇācinteyya saṃyuttaṃ;",
  "anya-e0604n:442": "Vande sāraguṇopetaṃ,",
  "anya-e0604n:443": "Tenamhā tulañāṇavā.",
  "anya-e0604n:444": "80.",
  "anya-e0604n:445": "Sajjhaṃ hemañca ratanaṃ,",
  "anya-e0604n:446": "Gehaṃ vatthañca bhojanaṃ;",
  "anya-e0604n:447": "Tadaññepi yathācittaṃ,",
  "anya-e0604n:448": "Māpeyyāhaṃ name jinaṃ.",
  "anya-e0604n:449": "81.",
  "anya-e0604n:450": "Sajjhaṃ hemañca ratanaṃ,",
  "anya-e0604n:451": "Gehaṃ vatthañca bhojanaṃ;",
  "anya-e0604n:452": "Tadaññepi yathācittaṃ,",
  "anya-e0604n:453": "Māpeyyaṃ kammajiddhiyā.",
  "anya-e0604n:454": "82.",
  "anya-e0604n:455": "Sattabojjhaṅga ratano,",
  "anya-e0604n:456": "Saddhādiratano muni;",
  "anya-e0604n:457": "Satippabhutiratano,",
  "anya-e0604n:458": "Vande taṃ purisuttamaṃ.",
  "anya-e0604n:459": "83.",
  "anya-e0604n:460": "Cakkādisattaratanaṃ,",
  "anya-e0604n:461": "Muttādiratanaṃ subhaṃ;",
  "anya-e0604n:462": "Vatthappabhutiratanaṃ,",
  "anya-e0604n:463": "Chandakkhaṇe labhāmahaṃ.",
  "anya-e0604n:464": "84.",
  "anya-e0604n:465": "Sampuṇṇacittasaṅkappo,",
  "anya-e0604n:466": "Yathākaṅkhitamāpako,",
  "anya-e0604n:467": "Kenacinabhibhūto yo,",
  "anya-e0604n:468": "Sabbābhibhū name jinaṃ.",
  "anya-e0604n:469": "85.",
  "anya-e0604n:470": "Sampuṇṇacittasaṅkappo,",
  "anya-e0604n:471": "Yathākaṅkhitamāpako,",
  "anya-e0604n:472": "Kenacinabhibhūtamhi,",
  "anya-e0604n:473": "Sabbābhibhū bhave bhave.",
  "anya-e0604n:474": "86.",
  "anya-e0604n:475": "Dasa chaddantarājāva,",
  "anya-e0604n:476": "Dasa kāyabalo muni;",
  "anya-e0604n:477": "Dasa ñāṇabbalotulyo,",
  "anya-e0604n:478": "Vande taṃ samaṇuttamaṃ.",
  "anya-e0604n:479": "87.",
  "anya-e0604n:480": "Atikāyajavo buddho,",
  "anya-e0604n:481": "Raññājavanahaṃsato;",
  "anya-e0604n:482": "Kā kathā ñāṇavegassa,",
  "anya-e0604n:483": "Vande taṃ samaṇuttamaṃ.",
  "anya-e0604n:484": "88.",
  "anya-e0604n:485": "Chaddantanāgarājāva,",
  "anya-e0604n:486": "Bhave bhave mahabbalo;",
  "anya-e0604n:487": "Rājājavanahaṃsova,",
  "anya-e0604n:488": "Paramaggajavo bhave.",
  "anya-e0604n:489": "89.",
  "anya-e0604n:490": "Udakā kāsacārī ca,",
  "anya-e0604n:491": "Mahinimujjako jino;",
  "anya-e0604n:492": "Māpako ca yathā cittaṃ,",
  "anya-e0604n:493": "Vande taṃ ñeyyapāraguṃ.",
  "anya-e0604n:494": "90.",
  "anya-e0604n:495": "Udakā kāsacārī ca,",
  "anya-e0604n:496": "Mahinimujjako bhave;",
  "anya-e0604n:497": "Māpako ca yathācittaṃ,",
  "anya-e0604n:498": "Bhavaso kammajiddhiyā.",
  "anya-e0604n:499": "91.",
  "anya-e0604n:500": "Gāme vane ca sabbattha,",
  "anya-e0604n:501": "Devehi manussehi ca;",
  "anya-e0604n:502": "Ābhatānantalābhassa,",
  "anya-e0604n:503": "Sadā yassa mahesino;",
  "anya-e0604n:504": "Pattalābhaggataṃ lokaṃ,",
  "anya-e0604n:505": "Taṃ vande munipuṅgavaṃ.",
  "anya-e0604n:506": "92.",
  "anya-e0604n:507": "Gāme vane ca sabbattha,",
  "anya-e0604n:508": "Devehi manussehi ca;",
  "anya-e0604n:509": "Ābhatā nantalābho me,",
  "anya-e0604n:510": "Sadā hotu bhavābhave.",
  "anya-e0604n:511": "93.",
  "anya-e0604n:512": "Puññassimassa tejena,",
  "anya-e0604n:513": "Yathā cittaṃ samijjhatu;",
  "anya-e0604n:514": "Sabbicchā sabbacintā ca,",
  "anya-e0604n:515": "Khippaṃ me jātijātiyaṃ.",
  "anya-e0604n:516": "94.",
  "anya-e0604n:517": "Natthīti vacanaṃ dukkhaṃ,",
  "anya-e0604n:518": "Dehīti vacanaṃ tathā;",
  "anya-e0604n:519": "Tasmā natthīti dehīti,",
  "anya-e0604n:520": "Mā me hotu bhavābhave.",
  "anya-e0604n:521": "95.",
  "anya-e0604n:522": "Sabbaṃ paravasaṃ dukkhaṃ,",
  "anya-e0604n:523": "Sabbamissariyaṃ sukhaṃ;",
  "anya-e0604n:524": "Sabbaṃ paravasamatthu,",
  "anya-e0604n:525": "Sabbamissariyaṃ bhave.",
  "anya-e0604n:526": "96.",
  "anya-e0604n:527": "Vitakkento bhajjakāyo,",
  "anya-e0604n:528": "Sabbāvudha vāraṇo ca;",
  "anya-e0604n:529": "Chaddanta vāraṇabalo,",
  "anya-e0604n:530": "Bhaveyyaṃ jātijā tiyaṃ.",
  "anya-e0604n:531": "97.",
  "anya-e0604n:532": "Subhalakkhaṇasampanno,",
  "anya-e0604n:533": "Suvaṇṇavaṇṇavā bhave;",
  "anya-e0604n:534": "Brahmassaro karavika,",
  "anya-e0604n:535": "Bhāṇī ca jātijātiyaṃ.",
  "anya-e0604n:536": "98.",
  "anya-e0604n:537": "Bhūrimapañño sippānaṃ,",
  "anya-e0604n:538": "Sabbesaṃ kusalo bhave;",
  "anya-e0604n:539": "Visajjetuṃ samatthova,",
  "anya-e0604n:540": "Sabbapucchānaṃ ṭhānaso.",
  "anya-e0604n:541": "99.",
  "anya-e0604n:542": "Verādhaṃsīya bhogā ca,",
  "anya-e0604n:543": "Anantākhīṇa bhogavā;",
  "anya-e0604n:544": "Anantābhajja pariso,",
  "anya-e0604n:545": "Bhavaso pāpuṇe sivaṃ.",
  "anya-e0604n:546": "100.",
  "anya-e0604n:547": "Mā nasseyyaṃ pasayheyyaṃ,",
  "anya-e0604n:548": "Ussukkehi pyahaṃ saha;",
  "anya-e0604n:549": "Pañcaverehi koṭīhi,",
  "anya-e0604n:550": "Bhaṇḍaṃ vā akātukāmitaṃ.",
  "anya-e0604n:551": "101.",
  "anya-e0604n:552": "Meghaṃ vātañca ratanaṃ,",
  "anya-e0604n:553": "Dhaññaṃ vatthañca bhojanaṃ;",
  "anya-e0604n:554": "Sabbicchitaṃ tadaññampi,",
  "anya-e0604n:555": "Māpeyyaṃ kammajiddhiyā.",
  "anya-e0604n:556": "102.",
  "anya-e0604n:557": "Puññenetena nibbānaṃ,",
  "anya-e0604n:558": "Santaṃ pappomi tāvatā;",
  "anya-e0604n:559": "Bhaveyyaṃ sabbajātīsu,",
  "anya-e0604n:560": "Catusampattiyā sadā;",
  "anya-e0604n:561": "Catucakkena sampanno,",
  "anya-e0604n:562": "Saddhammehi ca sattahi.",
  "anya-e0604n:563": "103.",
  "anya-e0604n:564": "Sammādiṭṭhi vasupete,",
  "anya-e0604n:565": "Kulamhi seṭṭhasammate,",
  "anya-e0604n:566": "Sabbasakkata saṃsuddhe;",
  "anya-e0604n:567": "Bhave tihetusandhiko.",
  "anya-e0604n:568": "104.",
  "anya-e0604n:569": "Ghāsacchādanaṃ bhogañca,",
  "anya-e0604n:570": "Neva hatthena kātuna,",
  "anya-e0604n:571": "Bhuñjeyyamiddhiyātveva,",
  "anya-e0604n:572": "Māpetvā yāvadatthakaṃ.",
  "anya-e0604n:573": "105.",
  "anya-e0604n:574": "Manussānaṃ atikkamma,",
  "anya-e0604n:575": "Devānaṃ viya bhojanaṃ,",
  "anya-e0604n:576": "Vatthaṃ bhogo ca me hontu,",
  "anya-e0604n:577": "Parivārātisundarā.",
  "anya-e0604n:578": "106.",
  "anya-e0604n:579": "Dasāsuttaṃpi me bhogaṃ,",
  "anya-e0604n:580": "Pañcaverā bhave bhave;",
  "anya-e0604n:581": "Māgaṇheyyuñca nasseyyuṃ,",
  "anya-e0604n:582": "Asoko bhogahetume.",
  "anya-e0604n:583": "107.",
  "anya-e0604n:584": "Sabbaṅgasubha sampanno,",
  "anya-e0604n:585": "Siṅgīnikkha savaṇṇa vā;",
  "anya-e0604n:586": "Bāttiṃsa lakkhaṇūpeto,",
  "anya-e0604n:587": "Atappana rūpa vā bhave.",
  "anya-e0604n:588": "108.",
  "anya-e0604n:589": "Kāyo candanagandho ca,",
  "anya-e0604n:590": "Mukhaṃ uppalagandhikaṃ;",
  "anya-e0604n:591": "Aṭṭhaṅgiko karavika,",
  "anya-e0604n:592": "Mañjūghoso ca me hotu.",
  "anya-e0604n:593": "109.",
  "anya-e0604n:594": "Tikkha gambhīra pañño ca,",
  "anya-e0604n:595": "Hāsātijavapaññavā,",
  "anya-e0604n:596": "Bhūri nibbedha pañño ca;",
  "anya-e0604n:597": "Sabbapañhavisajjano.",
  "anya-e0604n:598": "111.",
  "anya-e0604n:599": "Anto soḷasavassassa,",
  "anya-e0604n:600": "Tipeṭakadharo bhave;",
  "anya-e0604n:601": "Sabbakammesu sippesu,",
  "anya-e0604n:602": "Vijjāṭhānesu pāragū."
}