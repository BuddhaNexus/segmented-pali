{
  "anya-e0604n:0": " Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e0604n:1": "Suttavandanā",
  "anya-e0604n:2": "1.",
  "anya-e0604n:3": " Dhammattha desanā vedha,",
  "anya-e0604n:4": "Catugambhīra duddasaṃ;",
  "anya-e0604n:5": "Dhammaṃ lokassa desentaṃ,",
  "anya-e0604n:6": "Dhammarājaṃ namāmahaṃ;",
  "anya-e0604n:7": "2.",
  "anya-e0604n:8": " Buddho paccekabuddho ca,",
  "anya-e0604n:9": "Arahā cakkavattīti;",
  "anya-e0604n:10": "Thūpāthūpārahaṃ vande,",
  "anya-e0604n:11": "Catuthūpārahesu taṃ;",
  "anya-e0604n:12": "3.",
  "anya-e0604n:13": " Petaseyyo kāmabhogī,",
  "anya-e0604n:14": "Sīhaseyyo tathāgato;",
  "anya-e0604n:15": "Sayantaṃ catuseyyesu,",
  "anya-e0604n:16": "Catutthena name jinaṃ;",
  "anya-e0604n:17": "4.",
  "anya-e0604n:18": " Aṇḍajā jalābujā ca;",
  "anya-e0604n:19": "Saṃsedajo papātikā;",
  "anya-e0604n:20": "Catuyoni pariccheda,",
  "anya-e0604n:21": "Ñāṇavantaṃ name jinaṃ;",
  "anya-e0604n:22": "5.",
  "anya-e0604n:23": " Sutabuddhonubuddho ca,",
  "anya-e0604n:24": "Pacceko jinabuddhoti;",
  "anya-e0604n:25": "Catutthaṃ catubuddhesu,",
  "anya-e0604n:26": "Vandāmi lokanāyakaṃ;",
  "anya-e0604n:27": "6.",
  "anya-e0604n:28": " Rāhuggaṃ attabhāgīnaṃ,",
  "anya-e0604n:29": "Mandhātā kāmabhogīnaṃ;",
  "anya-e0604n:30": "Māro adhipateyyānaṃ,",
  "anya-e0604n:31": "Buddho loke sadevake;",
  "anya-e0604n:32": "Catuagga paññattīnaṃ,",
  "anya-e0604n:33": "Aggapattaṃ name jinaṃ.",
  "anya-e0604n:34": "7.",
  "anya-e0604n:35": " Dānaṃ piyakathā attha,",
  "anya-e0604n:36": "Cariyā sadisattatā;",
  "anya-e0604n:37": "Catusaṅgaha vatthūhi,",
  "anya-e0604n:38": "Asaṅkhiyesu jātisu;",
  "anya-e0604n:39": "Lokassa saṅgahetāraṃ,",
  "anya-e0604n:40": "Vandāmi lokanāyakaṃ;",
  "anya-e0604n:41": "8.",
  "anya-e0604n:42": " Lobho doso ca mohoti,",
  "anya-e0604n:43": "Anto paccatthike tayo;",
  "anya-e0604n:44": "Dujjaye sabbaverīhi,",
  "anya-e0604n:45": "Sañjitaṃpi sudhiṃ name;",
  "anya-e0604n:46": "9.",
  "anya-e0604n:47": " Jāti puñña mahattāni,",
  "anya-e0604n:48": "Guṇamahattamuttamaṃ;",
  "anya-e0604n:49": "Timahatthehi sampannaṃ,",
  "anya-e0604n:50": "Vandāmi lokanāyakaṃ;",
  "anya-e0604n:51": "10.",
  "anya-e0604n:52": " Hetu phala sampadāyo,",
  "anya-e0604n:53": "Sattupakārasampadā;",
  "anya-e0604n:54": "Tisampadāhi sampannaṃ,",
  "anya-e0604n:55": "Vandāmi lokanāyakaṃ.",
  "anya-e0604n:56": "11.",
  "anya-e0604n:57": " Ñāṇappahānānubhāva,",
  "anya-e0604n:58": "Surūpakārasampadā;",
  "anya-e0604n:59": "Catuphala sampadāhi,",
  "anya-e0604n:60": "Taṃ sampannaṃ name jinaṃ.",
  "anya-e0604n:61": "12.",
  "anya-e0604n:62": " Āsayo ca payogoti,",
  "anya-e0604n:63": "Sattupakāra sampadā;",
  "anya-e0604n:64": "Duvidhā yassa sampannā,",
  "anya-e0604n:65": "Sammā maṃ pātu so jino.",
  "anya-e0604n:66": "13.",
  "anya-e0604n:67": " Mahosadhe vessantare,",
  "anya-e0604n:68": "Pacchimabhavike tathā;",
  "anya-e0604n:69": "Tijātīsu name vācaṃ,",
  "anya-e0604n:70": "Bhāsentaṃ jāyanakkhaṇe.",
  "anya-e0604n:71": "14.",
  "anya-e0604n:72": " Nemimhi sādhine ceva,",
  "anya-e0604n:73": "Mandhātari ca guttile;",
  "anya-e0604n:74": "Catūsu manujattena,",
  "anya-e0604n:75": "Devalokaṃ gataṃ name.",
  "anya-e0604n:76": "15.",
  "anya-e0604n:77": " Saṅkhāro rūpavikāro,",
  "anya-e0604n:78": "Lakkhaṇaṃ vānanissaṭaṃ;",
  "anya-e0604n:79": "Nibbānaṃ paññatticeti,",
  "anya-e0604n:80": "Pañca ñeyyantaguṃ name.",
  "anya-e0604n:81": "16.",
  "anya-e0604n:82": " Putta dāra pariccāgā,",
  "anya-e0604n:83": "Rajjaṅgaṃ jīva cāgīti;",
  "anya-e0604n:84": "Pañca mahāpariccāge,",
  "anya-e0604n:85": "Karaṃ sudukkare name.",
  "anya-e0604n:86": "17.",
  "anya-e0604n:87": " Rūpañca vedanā saññā,",
  "anya-e0604n:88": "Saṅkhārā viññāṇantime;",
  "anya-e0604n:89": "Pañcakkhandhe parijānaṃ,",
  "anya-e0604n:90": "Tipariññāhi taṃ name.",
  "anya-e0604n:91": "18.",
  "anya-e0604n:92": " Manussa deva gatiyo,",
  "anya-e0604n:93": "Peto ca nirayo tathā;",
  "anya-e0604n:94": "Tiracchānoti pañcetā,",
  "anya-e0604n:95": "Chindantaṃ gatiyo name;",
  "anya-e0604n:96": "19.",
  "anya-e0604n:97": " Devaputto kileso ca,",
  "anya-e0604n:98": "Abhisaṅkhāra mārako;",
  "anya-e0604n:99": "Khandho maccūti pañcete,",
  "anya-e0604n:100": "Māre vijitavaṃ name.",
  "anya-e0604n:101": "20.",
  "anya-e0604n:102": " Purebhattaṃ pacchābhattaṃ,",
  "anya-e0604n:103": "Purimañceva majjhimaṃ;",
  "anya-e0604n:104": "Pacchimanti sadā pañca,",
  "anya-e0604n:105": "Buddhakiccaṃ karaṃ name.",
  "anya-e0604n:106": "21.",
  "anya-e0604n:107": " Rūpā saddā gandhā rasā,",
  "anya-e0604n:108": "Phoṭṭhabbā ca manoramā;",
  "anya-e0604n:109": "Pañca kāmaguṇe hitvā,",
  "anya-e0604n:110": "Kheḷaṃva niggataṃ name.",
  "anya-e0604n:111": "22.",
  "anya-e0604n:112": " Rāgasallaṃ dosasallaṃ,",
  "anya-e0604n:113": "Mohā māno ca diṭṭhīti;",
  "anya-e0604n:114": "Vande vajjātivajjantaṃ,",
  "anya-e0604n:115": "Pañcasallapanūdanaṃ.",
  "anya-e0604n:116": "23.",
  "anya-e0604n:117": " Dīgho majjhima saṃyuttā,",
  "anya-e0604n:118": "Aṅguttaro ca khuddako;",
  "anya-e0604n:119": "Iti pañca nikāyehi,",
  "anya-e0604n:120": "Suvinentaṃ pajaṃ name.",
  "anya-e0604n:121": "24.",
  "anya-e0604n:122": " Issariyaṃ yaso dhammo,",
  "anya-e0604n:123": "Kāmo sirī payattīti;",
  "anya-e0604n:124": "Bhagyehi chahi sampannaṃ,",
  "anya-e0604n:125": "Vandāmi lokanāyakaṃ.",
  "anya-e0604n:126": "25.",
  "anya-e0604n:127": " Piyāpiyaṃ bhūtābhūtaṃ,",
  "anya-e0604n:128": "Atthānatthaṃ yathārahaṃ;",
  "anya-e0604n:129": "Missā chasu suvācaṃva,",
  "anya-e0604n:130": "Bhāsantaṃ atthakaṃ name.",
  "anya-e0604n:131": "26.",
  "anya-e0604n:132": " Alaso ca pamādo ca,",
  "anya-e0604n:133": "Anuṭṭhānaṃ asaṃyamo;",
  "anya-e0604n:134": "Niddā tantīti chiddehi,",
  "anya-e0604n:135": "Chahi muttaṃ sadā name.",
  "anya-e0604n:136": "27.",
  "anya-e0604n:137": " Mahākaruṇāsamāpatti,",
  "anya-e0604n:138": "Yamakappāṭihāriyaṃ;",
  "anya-e0604n:139": "Āsayānusaye ñāṇaṃ,",
  "anya-e0604n:140": "Indriyāna paropare;",
  "anya-e0604n:141": "Sabbaññutā nāvaraṇaṃ,",
  "anya-e0604n:142": "Chā sādhāraṇikaṃ name.",
  "anya-e0604n:143": "28.",
  "anya-e0604n:144": " Kāmacchando ca byāpādo,",
  "anya-e0604n:145": "Thinamiddhañca saṃsayo;",
  "anya-e0604n:146": "Avijjuddhacca kukkuccaṃ,",
  "anya-e0604n:147": "Chavinīvaraṇaṃ name.",
  "anya-e0604n:148": "29.",
  "anya-e0604n:149": " Iddhividhaṃ dibbasotaṃ,",
  "anya-e0604n:150": "Paracitta vijānanā;",
  "anya-e0604n:151": "Pubbenivāsānussati,",
  "anya-e0604n:152": "Dibbacakkhāsavakkhayo;",
  "anya-e0604n:153": "Chaḷabhiññāhi sampannaṃ,",
  "anya-e0604n:154": "Vandāmi purisuttamaṃ.",
  "anya-e0604n:155": "30.",
  "anya-e0604n:156": " Nīla pītā ca odātā,",
  "anya-e0604n:157": "Mañjiṭṭhā ca pabhassarā;",
  "anya-e0604n:158": "Lohitāti cha raṃsīhī,",
  "anya-e0604n:159": "Vijjotantaṃ sadā name.",
  "anya-e0604n:160": "31.",
  "anya-e0604n:161": " Assādādīnavo ceva,",
  "anya-e0604n:162": "Nissaraṇaphalaṃ tathā;",
  "anya-e0604n:163": "Upāyāṇattīti chadhā,",
  "anya-e0604n:164": "Saddhammadesakaṃ name.",
  "anya-e0604n:165": "32.",
  "anya-e0604n:166": " Rāgo doso ca moho ca,",
  "anya-e0604n:167": "Vitakkacariyā tathā;",
  "anya-e0604n:168": "Saddhā buddhīti bhājentaṃ,",
  "anya-e0604n:169": "Tā chaḷecariyā name.",
  "anya-e0604n:170": "33.",
  "anya-e0604n:171": " Rajataṃ kanakaṃ muttā,",
  "anya-e0604n:172": "Maṇi veḷuriyāni ca;",
  "anya-e0604n:173": "Vajirañca pavāḷanti,",
  "anya-e0604n:174": "Seṭṭhi sattadhano viya.",
  "anya-e0604n:175": "34.",
  "anya-e0604n:176": " Saddhāsīlaṃ sutaṃcāgo,",
  "anya-e0604n:177": "Hirī ottappiyaṃ dhanaṃ,",
  "anya-e0604n:178": "Paññādhananti sambuddhaṃ,",
  "anya-e0604n:179": "Vande sattamahādhanaṃ.",
  "anya-e0604n:180": "35.",
  "anya-e0604n:181": " Cakkaṃ hatthi asso maṇi,",
  "anya-e0604n:182": "Gahapatitthiyo tathā;",
  "anya-e0604n:183": "Supariṇāyakocāti,",
  "anya-e0604n:184": "Sattaratano cakkavattīva.",
  "anya-e0604n:185": "36.",
  "anya-e0604n:186": " Sati dhammavicayo ca,",
  "anya-e0604n:187": "Tatheva vīriyaṃ pīti;",
  "anya-e0604n:188": "Passaddhi samādhupekkhā,",
  "anya-e0604n:189": "Saddhamma cakkavattikaṃ;",
  "anya-e0604n:190": "Satta bojjhaṅgaratanaṃ,",
  "anya-e0604n:191": "Namāmi purisuttamaṃ.",
  "anya-e0604n:192": "37.",
  "anya-e0604n:193": " Yugandharo īsadharo,",
  "anya-e0604n:194": "Karavīko sudassano;",
  "anya-e0604n:195": "Nemindharo vinatako,",
  "anya-e0604n:196": "Assakaṇṇo mahānage;",
  "anya-e0604n:197": "Sattete hattharūpaṃva,",
  "anya-e0604n:198": "Sampassantaṃ jinaṃ name.",
  "anya-e0604n:199": "38.",
  "anya-e0604n:200": " Kaṇṇamuṇḍo anotatto,",
  "anya-e0604n:201": "Kuṇālo rathakārako;",
  "anya-e0604n:202": "Sīhappapāta chaddantā,",
  "anya-e0604n:203": "Mandākinī mahāsare;",
  "anya-e0604n:204": "Sattete sabbathā passaṃ,",
  "anya-e0604n:205": "Diṭṭheva pātikaṃ name.",
  "anya-e0604n:206": "39.",
  "anya-e0604n:207": " Manussattaṃ liṅgasampatti,",
  "anya-e0604n:208": "Hetu satthāradassanaṃ;",
  "anya-e0604n:209": "Pabbajjā guṇasampatti,",
  "anya-e0604n:210": "Adhikāro ca chandatā;",
  "anya-e0604n:211": "Aṭṭhadhamma samodhānā,",
  "anya-e0604n:212": "Bhinīhārantaguṃ name.",
  "anya-e0604n:213": "40.",
  "anya-e0604n:214": " Visaṭṭhaṃ mañju viññeyyaṃ,",
  "anya-e0604n:215": "Savanīyāvisāriṇā;",
  "anya-e0604n:216": "Gambhīro binduninnādo,",
  "anya-e0604n:217": "Aṭṭhaṅgikasaraṃ name.",
  "anya-e0604n:218": "41.",
  "anya-e0604n:219": " Cattāri rūpajhānāni,",
  "anya-e0604n:220": "Tathevārūpajhānāni;",
  "anya-e0604n:221": "Tā samāpattiyo aṭṭha,",
  "anya-e0604n:222": "Sevantaṃ ruciyā name.",
  "anya-e0604n:223": "42.",
  "anya-e0604n:224": " Aṇimā laṅghimā kammaṃ,",
  "anya-e0604n:225": "Mahimā pattimā tathā;",
  "anya-e0604n:226": "Īsitā vasitā ceva,",
  "anya-e0604n:227": "Yatthakāmāvasāyitā;",
  "anya-e0604n:228": "Aṭṭhissariya puṇṇattā,",
  "anya-e0604n:229": "Issarātissaraṃ name.",
  "anya-e0604n:230": "43.",
  "anya-e0604n:231": " Manomayiddhiñāṇañca,",
  "anya-e0604n:232": "Chaḷabhiññā vipassanā;",
  "anya-e0604n:233": "Aṭṭhavijjāhi sampannaṃ,",
  "anya-e0604n:234": "Vande tilokaketukaṃ.",
  "anya-e0604n:235": "44.",
  "anya-e0604n:236": " Jāti jarā rujā kālo,",
  "anya-e0604n:237": "Caturāpāyadukkhāti;",
  "anya-e0604n:238": "Aṭṭha saṃvegavatthūni,",
  "anya-e0604n:239": "Bhāsantaṃ vividhaṃ name.",
  "anya-e0604n:240": "45.",
  "anya-e0604n:241": " Suttaṅgaṃ veyyākaraṇaṃ,",
  "anya-e0604n:242": "Geyyaṃ gāthā ca jātakaṃ;",
  "anya-e0604n:243": "Abbhūtadhamma vedallā,",
  "anya-e0604n:244": "Udānamiti vuttakaṃ;",
  "anya-e0604n:245": "Navaṅgaṃ sabbalokassa,",
  "anya-e0604n:246": "Dadantaṃ sāsanaṃ name.",
  "anya-e0604n:247": "46.",
  "anya-e0604n:248": " Yā samāpattiyo aṭṭha,",
  "anya-e0604n:249": "Nirodho cāti te nava,",
  "anya-e0604n:250": "Anupubbavihāre taṃ,",
  "anya-e0604n:251": "Sevantaṃ ruciyā name.",
  "anya-e0604n:252": "47.",
  "anya-e0604n:253": " Ekattañceva nānattaṃ,",
  "anya-e0604n:254": "Kāyasaññīhi yogato,",
  "anya-e0604n:255": "Cattāro tathāruppā ca,",
  "anya-e0604n:256": "Asaññīcāti te nava,",
  "anya-e0604n:257": "Sattavāse vibhajitvā,",
  "anya-e0604n:258": "Pakāsentaṃ name jinaṃ.",
  "anya-e0604n:259": "48.",
  "anya-e0604n:260": " Seyyassa sadiso seyyo,",
  "anya-e0604n:261": "Hīnoti tividho vidho,",
  "anya-e0604n:262": "Tathā sadisahīnānaṃ,",
  "anya-e0604n:263": "Navavidhe nudaṃ name.",
  "anya-e0604n:264": "49.",
  "anya-e0604n:265": " Anatthaṃ me carati ca,",
  "anya-e0604n:266": "Tathā cari carissati,",
  "anya-e0604n:267": "Mittassa arinotvatthaṃ,",
  "anya-e0604n:268": "Navāghāte jahaṃ name.",
  "anya-e0604n:269": "50.",
  "anya-e0604n:270": " Dānaṃ sīlañca nikkhamaṃ,",
  "anya-e0604n:271": "Paññā vīriyapañcamaṃ,",
  "anya-e0604n:272": "Khantī saccamadhiṭṭhānaṃ,",
  "anya-e0604n:273": "Mettupekkhāti tādasa,",
  "anya-e0604n:274": "Pūretvā pāramī vande,",
  "anya-e0604n:275": "Sammāsambodhimajjhavaṃ;",
  "anya-e0604n:276": "51.",
  "anya-e0604n:277": " Kāḷāvakañca gaṅgeyyaṃ,",
  "anya-e0604n:278": "Paṇḍaraṃ tamba piṅgalaṃ,",
  "anya-e0604n:279": "Gandha maṅgala hemañca,",
  "anya-e0604n:280": "Uposatha chaddantime.",
  "anya-e0604n:281": "52.",
  "anya-e0604n:282": " Dhāreti dasaposānaṃ,",
  "anya-e0604n:283": "Nāgo kāḷāvako balaṃ;",
  "anya-e0604n:284": "Sahassakoṭiposānaṃ;",
  "anya-e0604n:285": "Chaddantovāranuttamo.",
  "anya-e0604n:286": "53.",
  "anya-e0604n:287": " Dasa sahassakoṭīnaṃ,",
  "anya-e0604n:288": "Posānaṃ baladhāraṇaṃ;",
  "anya-e0604n:289": "Chaddantānaṃ dasannañca,",
  "anya-e0604n:290": "Dasakāyabalaṃ name.",
  "anya-e0604n:291": "54.",
  "anya-e0604n:292": " Ṭhānāṭhāne vipāke ca,",
  "anya-e0604n:293": "Magge sabbatthagāminaṃ;",
  "anya-e0604n:294": "Nānādhātūsu lokesu,",
  "anya-e0604n:295": "Adhimuttimhi pāṇinaṃ;",
  "anya-e0604n:296": "55.",
  "anya-e0604n:297": " Paropariyatte ñāṇaṃ,",
  "anya-e0604n:298": "Indriyānañca pāṇinaṃ;",
  "anya-e0604n:299": "Jhānādīsu ñāṇaṃ pubbe,",
  "anya-e0604n:300": "Nivāse dibbacakkhu ca;",
  "anya-e0604n:301": "Āsavakkhayañāṇanti,",
  "anya-e0604n:302": "Dasabalañāṇaṃ name.",
  "anya-e0604n:303": "56.",
  "anya-e0604n:304": " Mūlagandho sāro pheggu,",
  "anya-e0604n:305": "Taco papaṭiko raso;",
  "anya-e0604n:306": "Pattaṃ pupphaṃ phalañceva,",
  "anya-e0604n:307": "Gandhagandhoti saṃvaraṃ;",
  "anya-e0604n:308": "Sugandhaṃ dasagandhehi,",
  "anya-e0604n:309": "Vilimpantaṃ sadā name.",
  "anya-e0604n:310": "57.",
  "anya-e0604n:311": " Samādhi ñāṇavipphārā,",
  "anya-e0604n:312": "Adhiṭṭhānaṃ vikubbanā;",
  "anya-e0604n:313": "Manomayāriyā iddhi,",
  "anya-e0604n:314": "Tathā kammavipākajā.",
  "anya-e0604n:315": "58.",
  "anya-e0604n:316": " Vijjāmayā puññavato;",
  "anya-e0604n:317": "Payoga paccayiddhiti;",
  "anya-e0604n:318": "Aggapatta dasiddhīnaṃ,",
  "anya-e0604n:319": "Pāṭiheraṃ karaṃ name.",
  "anya-e0604n:320": "59.",
  "anya-e0604n:321": " Muttā maṇi veḷuriyaṃ,",
  "anya-e0604n:322": "Saṅkho sallo pavāḷakaṃ;",
  "anya-e0604n:323": "Suvaṇṇaṃ rajataṃ lohi,",
  "anya-e0604n:324": "Taṅko masāragallanti;",
  "anya-e0604n:325": "Dasadhā ratanehiggaṃ,",
  "anya-e0604n:326": "Bhogiṃ paññāmaṇiṃ name.",
  "anya-e0604n:327": "60.",
  "anya-e0604n:328": " Sithilaṃ dhanitaṃ dīghaṃ,",
  "anya-e0604n:329": "Rassañca garukaṃ lahu;",
  "anya-e0604n:330": "Niggahitaṃ vimuttañca,",
  "anya-e0604n:331": "Sambandhañca vavatthitaṃ;",
  "anya-e0604n:332": "Vivarantaṃ name dhammaṃ,",
  "anya-e0604n:333": "Dasabyañjanabheditaṃ.",
  "anya-e0604n:334": "61.",
  "anya-e0604n:335": " Pasādojo madhuratā,",
  "anya-e0604n:336": "Samatā sukhumālatā;",
  "anya-e0604n:337": "Silesodāratā kanti,",
  "anya-e0604n:338": "Atthabyatti samādhayo;",
  "anya-e0604n:339": "Dasa saddaguṇopetaṃ,",
  "anya-e0604n:340": "Dhammaṃ pātu karaṃ name.",
  "anya-e0604n:341": "62.",
  "anya-e0604n:342": " Lobho doso moho māno,",
  "anya-e0604n:343": "Uddhaccaṃ diṭṭhi saṃsayo;",
  "anya-e0604n:344": "Ahirikamanottappaṃ,",
  "anya-e0604n:345": "Thinanti sabba dāhake;",
  "anya-e0604n:346": "Kilese dasa te saddhiṃ,",
  "anya-e0604n:347": "Vāsanāya jahaṃ name.",
  "anya-e0604n:348": "63.",
  "anya-e0604n:349": " Saterā daṇḍamaṇikā,",
  "anya-e0604n:350": "Macchavilolagaggarā;",
  "anya-e0604n:351": "Sukkhāsani kapisisā,",
  "anya-e0604n:352": "Vicakkā sañña kukkuṭā;",
  "anya-e0604n:353": "Navāsanīhi phālentaṃ,",
  "anya-e0604n:354": "Tibbaṃ paññāsaniṃ name.",
  "anya-e0604n:355": "64.",
  "anya-e0604n:356": " Rodanā kodhāvudho ca,",
  "anya-e0604n:357": "Ujjhantissariyaṃ tathā;",
  "anya-e0604n:358": "Paṭisaṅkhānaṃ,",
  "anya-e0604n:359": "Khanti aṭṭhabalesu taṃ;",
  "anya-e0604n:360": "Pathavīsadisaṃ vande,",
  "anya-e0604n:361": "Aṭṭhamena vināyakaṃ.",
  "anya-e0604n:362": "65.",
  "anya-e0604n:363": " Kāmarogo ca paṭigho,",
  "anya-e0604n:364": "Māno diṭṭhi ca saṃsayo;",
  "anya-e0604n:365": "Bhavarāgo avijjāti,",
  "anya-e0604n:366": "Sattānusayinaṃ name.",
  "anya-e0604n:367": "66.",
  "anya-e0604n:368": " Titthiyāpakatimahāsāvakā,",
  "anya-e0604n:369": "Aggasāvakā paccekabuddhasambuddhā;",
  "anya-e0604n:370": "Visiṭṭhaṃ sabbathā chasu,",
  "anya-e0604n:371": "Pubbe nivāsa viññūsu;",
  "anya-e0604n:372": "Tilokamakuṭaṃ name.",
  "anya-e0604n:373": "67.",
  "anya-e0604n:374": " Buddhacakkhu samantā ca,",
  "anya-e0604n:375": "Ñāṇaṃ yaṃ dibbacakkhu ca;",
  "anya-e0604n:376": "Dhammoti pañca cakkhūhi,",
  "anya-e0604n:377": "Dassāviṃ mārajiṃ name.",
  "anya-e0604n:378": "68.",
  "anya-e0604n:379": " Khuddakā khaṇikā okkantikā ca,",
  "anya-e0604n:380": "Pharaṇā tathā ubbegā;",
  "anya-e0604n:381": "Pañca pītīhi pinentaṃ,",
  "anya-e0604n:382": "Ruciyā ca namāmahaṃ.",
  "anya-e0604n:383": "69.",
  "anya-e0604n:384": " Dukkhaṃ samudayasaccaṃ,",
  "anya-e0604n:385": "Nirodho maggasaccakaṃ;",
  "anya-e0604n:386": "Catusaccamabhijānaṃ,",
  "anya-e0604n:387": "Catukiccehi taṃ name.",
  "anya-e0604n:388": "70.",
  "anya-e0604n:389": " Pīḷanā saṅkhatattho ca,",
  "anya-e0604n:390": "Tapo vipariṇāmanā;",
  "anya-e0604n:391": "Dukkhasaccaṃ catukkehi,",
  "anya-e0604n:392": "Vibhattāviṃ muniṃ name.",
  "anya-e0604n:393": "71.",
  "anya-e0604n:394": " Āyūhanaṃ nidānañca,",
  "anya-e0604n:395": "Saṃyogo palibodhanaṃ;",
  "anya-e0604n:396": "Catukkehi samudayaṃ,",
  "anya-e0604n:397": "Vibhattāviṃ muniṃ name.",
  "anya-e0604n:398": "72.",
  "anya-e0604n:399": " Nissaraṇañca viveko,",
  "anya-e0604n:400": "Asaṅkhatāmataṃ tathā;",
  "anya-e0604n:401": "Nirodhañca catukkehi,",
  "anya-e0604n:402": "Vibhattāviṃ muniṃ name.",
  "anya-e0604n:403": "73.",
  "anya-e0604n:404": " Niyyāniko ca hetvattho,",
  "anya-e0604n:405": "Dassanādhipateyyakaṃ;",
  "anya-e0604n:406": "Maggasaccaṃ catukkehi,",
  "anya-e0604n:407": "Vibhattāviṃ muniṃ name.",
  "anya-e0604n:408": "74.",
  "anya-e0604n:409": " Satthako ca asammoho,",
  "anya-e0604n:410": "Sappāyo ceva gocaro;",
  "anya-e0604n:411": "Catusampajaññā vinā,",
  "anya-e0604n:412": "Bhāviṃ name tathāgataṃ.",
  "anya-e0604n:413": "75.",
  "anya-e0604n:414": " Pahinākhila dukkhāhaṃ,",
  "anya-e0604n:415": "Bhavasāgara pāraguṃ;",
  "anya-e0604n:416": "Vande sāraguṇopetaṃ,",
  "anya-e0604n:417": "Tenamhi bhavapārago.",
  "anya-e0604n:418": "76.",
  "anya-e0604n:419": " Rūpārūpavilāsagga,",
  "anya-e0604n:420": "Rūpācinteyya saṃyuttaṃ;",
  "anya-e0604n:421": "Vande sāraguṇopetaṃ,",
  "anya-e0604n:422": "Tenamhātularūpavā.",
  "anya-e0604n:423": "77.",
  "anya-e0604n:424": " Iddhi iddhi vilāsagga,",
  "anya-e0604n:425": "Iddhi cinteyya saṃyuttaṃ;",
  "anya-e0604n:426": "Vande sāraguṇepetaṃ,",
  "anya-e0604n:427": "Tenamhā tulaiddhimā.",
  "anya-e0604n:428": "78.",
  "anya-e0604n:429": " Vācā vācā vilāsagga,",
  "anya-e0604n:430": "Vācā cinteyya saṃyuttaṃ;",
  "anya-e0604n:431": "Vande sāraguṇopetaṃ,",
  "anya-e0604n:432": "Tenamhā tulavācako.",
  "anya-e0604n:433": "79.",
  "anya-e0604n:434": " Ñāṇa ñāṇa vilāsagga,",
  "anya-e0604n:435": "Ñāṇācinteyya saṃyuttaṃ;",
  "anya-e0604n:436": "Vande sāraguṇopetaṃ,",
  "anya-e0604n:437": "Tenamhā tulañāṇavā.",
  "anya-e0604n:438": "80.",
  "anya-e0604n:439": " Sajjhaṃ hemañca ratanaṃ,",
  "anya-e0604n:440": "Gehaṃ vatthañca bhojanaṃ;",
  "anya-e0604n:441": "Tadaññepi yathācittaṃ,",
  "anya-e0604n:442": "Māpeyyāhaṃ name jinaṃ.",
  "anya-e0604n:443": "81.",
  "anya-e0604n:444": " Sajjhaṃ hemañca ratanaṃ,",
  "anya-e0604n:445": "Gehaṃ vatthañca bhojanaṃ;",
  "anya-e0604n:446": "Tadaññepi yathācittaṃ,",
  "anya-e0604n:447": "Māpeyyaṃ kammajiddhiyā.",
  "anya-e0604n:448": "82.",
  "anya-e0604n:449": " Sattabojjhaṅga ratano,",
  "anya-e0604n:450": "Saddhādiratano muni;",
  "anya-e0604n:451": "Satippabhutiratano,",
  "anya-e0604n:452": "Vande taṃ purisuttamaṃ.",
  "anya-e0604n:453": "83.",
  "anya-e0604n:454": " Cakkādisattaratanaṃ,",
  "anya-e0604n:455": "Muttādiratanaṃ subhaṃ;",
  "anya-e0604n:456": "Vatthappabhutiratanaṃ,",
  "anya-e0604n:457": "Chandakkhaṇe labhāmahaṃ.",
  "anya-e0604n:458": "84.",
  "anya-e0604n:459": " Sampuṇṇacittasaṅkappo,",
  "anya-e0604n:460": "Yathākaṅkhitamāpako,",
  "anya-e0604n:461": "Kenacinabhibhūto yo,",
  "anya-e0604n:462": "Sabbābhibhū name jinaṃ.",
  "anya-e0604n:463": "85.",
  "anya-e0604n:464": " Sampuṇṇacittasaṅkappo,",
  "anya-e0604n:465": "Yathākaṅkhitamāpako,",
  "anya-e0604n:466": "Kenacinabhibhūtamhi,",
  "anya-e0604n:467": "Sabbābhibhū bhave bhave.",
  "anya-e0604n:468": "86.",
  "anya-e0604n:469": " Dasa chaddantarājāva,",
  "anya-e0604n:470": "Dasa kāyabalo muni;",
  "anya-e0604n:471": "Dasa ñāṇabbalotulyo,",
  "anya-e0604n:472": "Vande taṃ samaṇuttamaṃ.",
  "anya-e0604n:473": "87.",
  "anya-e0604n:474": " Atikāyajavo buddho,",
  "anya-e0604n:475": "Raññājavanahaṃsato;",
  "anya-e0604n:476": "Kā kathā ñāṇavegassa,",
  "anya-e0604n:477": "Vande taṃ samaṇuttamaṃ.",
  "anya-e0604n:478": "88.",
  "anya-e0604n:479": " Chaddantanāgarājāva,",
  "anya-e0604n:480": "Bhave bhave mahabbalo;",
  "anya-e0604n:481": "Rājājavanahaṃsova,",
  "anya-e0604n:482": "Paramaggajavo bhave.",
  "anya-e0604n:483": "89.",
  "anya-e0604n:484": " Udakā kāsacārī ca,",
  "anya-e0604n:485": "Mahinimujjako jino;",
  "anya-e0604n:486": "Māpako ca yathā cittaṃ,",
  "anya-e0604n:487": "Vande taṃ ñeyyapāraguṃ.",
  "anya-e0604n:488": "90.",
  "anya-e0604n:489": " Udakā kāsacārī ca,",
  "anya-e0604n:490": "Mahinimujjako bhave;",
  "anya-e0604n:491": "Māpako ca yathācittaṃ,",
  "anya-e0604n:492": "Bhavaso kammajiddhiyā.",
  "anya-e0604n:493": "91.",
  "anya-e0604n:494": " Gāme vane ca sabbattha,",
  "anya-e0604n:495": "Devehi manussehi ca;",
  "anya-e0604n:496": "Ābhatānantalābhassa,",
  "anya-e0604n:497": "Sadā yassa mahesino;",
  "anya-e0604n:498": "Pattalābhaggataṃ lokaṃ,",
  "anya-e0604n:499": "Taṃ vande munipuṅgavaṃ.",
  "anya-e0604n:500": "92.",
  "anya-e0604n:501": " Gāme vane ca sabbattha,",
  "anya-e0604n:502": "Devehi manussehi ca;",
  "anya-e0604n:503": "Ābhatā nantalābho me,",
  "anya-e0604n:504": "Sadā hotu bhavābhave.",
  "anya-e0604n:505": "93.",
  "anya-e0604n:506": " Puññassimassa tejena,",
  "anya-e0604n:507": "Yathā cittaṃ samijjhatu;",
  "anya-e0604n:508": "Sabbicchā sabbacintā ca,",
  "anya-e0604n:509": "Khippaṃ me jātijātiyaṃ.",
  "anya-e0604n:510": "94.",
  "anya-e0604n:511": " Natthīti vacanaṃ dukkhaṃ,",
  "anya-e0604n:512": "Dehīti vacanaṃ tathā;",
  "anya-e0604n:513": "Tasmā natthīti dehīti,",
  "anya-e0604n:514": "Mā me hotu bhavābhave.",
  "anya-e0604n:515": "95.",
  "anya-e0604n:516": " Sabbaṃ paravasaṃ dukkhaṃ,",
  "anya-e0604n:517": "Sabbamissariyaṃ sukhaṃ;",
  "anya-e0604n:518": "Sabbaṃ paravasamatthu,",
  "anya-e0604n:519": "Sabbamissariyaṃ bhave.",
  "anya-e0604n:520": "96.",
  "anya-e0604n:521": " Vitakkento bhajjakāyo,",
  "anya-e0604n:522": "Sabbāvudha vāraṇo ca;",
  "anya-e0604n:523": "Chaddanta vāraṇabalo,",
  "anya-e0604n:524": "Bhaveyyaṃ jātijā tiyaṃ.",
  "anya-e0604n:525": "97.",
  "anya-e0604n:526": " Subhalakkhaṇasampanno,",
  "anya-e0604n:527": "Suvaṇṇavaṇṇavā bhave;",
  "anya-e0604n:528": "Brahmassaro karavika,",
  "anya-e0604n:529": "Bhāṇī ca jātijātiyaṃ.",
  "anya-e0604n:530": "98.",
  "anya-e0604n:531": " Bhūrimapañño sippānaṃ,",
  "anya-e0604n:532": "Sabbesaṃ kusalo bhave;",
  "anya-e0604n:533": "Visajjetuṃ samatthova,",
  "anya-e0604n:534": "Sabbapucchānaṃ ṭhānaso.",
  "anya-e0604n:535": "99.",
  "anya-e0604n:536": " Verādhaṃsīya bhogā ca,",
  "anya-e0604n:537": "Anantākhīṇa bhogavā;",
  "anya-e0604n:538": "Anantābhajja pariso,",
  "anya-e0604n:539": "Bhavaso pāpuṇe sivaṃ.",
  "anya-e0604n:540": "100.",
  "anya-e0604n:541": " Mā nasseyyaṃ pasayheyyaṃ,",
  "anya-e0604n:542": "Ussukkehi pyahaṃ saha;",
  "anya-e0604n:543": "Pañcaverehi koṭīhi,",
  "anya-e0604n:544": "Bhaṇḍaṃ vā akātukāmitaṃ.",
  "anya-e0604n:545": "101.",
  "anya-e0604n:546": " Meghaṃ vātañca ratanaṃ,",
  "anya-e0604n:547": "Dhaññaṃ vatthañca bhojanaṃ;",
  "anya-e0604n:548": "Sabbicchitaṃ tadaññampi,",
  "anya-e0604n:549": "Māpeyyaṃ kammajiddhiyā.",
  "anya-e0604n:550": "102.",
  "anya-e0604n:551": " Puññenetena nibbānaṃ,",
  "anya-e0604n:552": "Santaṃ pappomi tāvatā;",
  "anya-e0604n:553": "Bhaveyyaṃ sabbajātīsu,",
  "anya-e0604n:554": "Catusampattiyā sadā;",
  "anya-e0604n:555": "Catucakkena sampanno,",
  "anya-e0604n:556": "Saddhammehi ca sattahi.",
  "anya-e0604n:557": "103.",
  "anya-e0604n:558": " Sammādiṭṭhi vasupete,",
  "anya-e0604n:559": "Kulamhi seṭṭhasammate,",
  "anya-e0604n:560": "Sabbasakkata saṃsuddhe;",
  "anya-e0604n:561": "Bhave tihetusandhiko.",
  "anya-e0604n:562": "104.",
  "anya-e0604n:563": " Ghāsacchādanaṃ bhogañca,",
  "anya-e0604n:564": "Neva hatthena kātuna,",
  "anya-e0604n:565": "Bhuñjeyyamiddhiyātveva,",
  "anya-e0604n:566": "Māpetvā yāvadatthakaṃ.",
  "anya-e0604n:567": "105.",
  "anya-e0604n:568": " Manussānaṃ atikkamma,",
  "anya-e0604n:569": "Devānaṃ viya bhojanaṃ,",
  "anya-e0604n:570": "Vatthaṃ bhogo ca me hontu,",
  "anya-e0604n:571": "Parivārātisundarā.",
  "anya-e0604n:572": "106.",
  "anya-e0604n:573": " Dasāsuttaṃpi me bhogaṃ,",
  "anya-e0604n:574": "Pañcaverā bhave bhave;",
  "anya-e0604n:575": "Māgaṇheyyuñca nasseyyuṃ,",
  "anya-e0604n:576": "Asoko bhogahetume.",
  "anya-e0604n:577": "107.",
  "anya-e0604n:578": " Sabbaṅgasubha sampanno,",
  "anya-e0604n:579": "Siṅgīnikkha savaṇṇa vā;",
  "anya-e0604n:580": "Bāttiṃsa lakkhaṇūpeto,",
  "anya-e0604n:581": "Atappana rūpa vā bhave.",
  "anya-e0604n:582": "108.",
  "anya-e0604n:583": " Kāyo candanagandho ca,",
  "anya-e0604n:584": "Mukhaṃ uppalagandhikaṃ;",
  "anya-e0604n:585": "Aṭṭhaṅgiko karavika,",
  "anya-e0604n:586": "Mañjūghoso ca me hotu.",
  "anya-e0604n:587": "109.",
  "anya-e0604n:588": " Tikkha gambhīra pañño ca,",
  "anya-e0604n:589": "Hāsātijavapaññavā,",
  "anya-e0604n:590": "Bhūri nibbedha pañño ca;",
  "anya-e0604n:591": "Sabbapañhavisajjano.",
  "anya-e0604n:592": "111.",
  "anya-e0604n:593": " Anto soḷasavassassa,",
  "anya-e0604n:594": "Tipeṭakadharo bhave;",
  "anya-e0604n:595": "Sabbakammesu sippesu,",
  "anya-e0604n:596": "Vijjāṭhānesu pāragū."
}