{
  "anya-e0605n11:0": "Apunarāvattigamanadīpanīyamakagāthā",
  "anya-e0605n11:1": "78.",
  "anya-e0605n11:2": " Anantakālopacitena tena,",
  "anya-e0605n11:3": "Puññena nibbattavimānayāne;",
  "anya-e0605n11:4": "Tasmiṃ dine jātasutaṃ pajāpatiṃ,",
  "anya-e0605n11:5": "Hitvā gato so sugato gato va.",
  "anya-e0605n11:6": "79.",
  "anya-e0605n11:7": " Taṃ jīvamānaṃ pitarañca mātaraṃ,",
  "anya-e0605n11:8": "Te ñātake tādisiyo ca itthiyo;",
  "anya-e0605n11:9": "Te tādise rammakare nikete,",
  "anya-e0605n11:10": "Hitvā gato so sugato gato va.",
  "anya-e0605n11:11": "80.",
  "anya-e0605n11:12": " Khomañca pattuṇṇadukūlacīnaṃ,",
  "anya-e0605n11:13": "Sakāsikaṃ sādhusugandhavāsitaṃ;",
  "anya-e0605n11:14": "Nivāsito sobhati vāsavo va,",
  "anya-e0605n11:15": "Hitvā gato so sugato gato va.",
  "anya-e0605n11:16": "81.",
  "anya-e0605n11:17": " Vidhippakāsā nidhiyo catasso,",
  "anya-e0605n11:18": "Samuggatā bhūtadharā vasundharā;",
  "anya-e0605n11:19": "Sattāvasattāvasudhā sudhāsā,",
  "anya-e0605n11:20": "Hitvā gato so sugato gato va.",
  "anya-e0605n11:21": "82.",
  "anya-e0605n11:22": " Suvaṇṇathāle satarājike subhe,",
  "anya-e0605n11:23": "Sādhuṃ sugandhaṃ sucisālibhojanaṃ;",
  "anya-e0605n11:24": "Bhutvā savāsīhi vilāsinīhi,",
  "anya-e0605n11:25": "Hitvā gata so sugato gato va.",
  "anya-e0605n11:26": "83.",
  "anya-e0605n11:27": " Manuññagandhena asuññagandho,",
  "anya-e0605n11:28": "Sugandhagandhena vilittagatto;",
  "anya-e0605n11:29": "Sugandhavātena suvijjitaṅgo,",
  "anya-e0605n11:30": "Hitvā gato so sugato gato va.",
  "anya-e0605n11:31": "84.",
  "anya-e0605n11:32": " Sulakkhaṇe hevabhilakkhitaṅgo,",
  "anya-e0605n11:33": "Pasādhito devapasādhanena;",
  "anya-e0605n11:34": "Virocamāno samarājinīhi,",
  "anya-e0605n11:35": "Hitvā gato so sugato gato va.",
  "anya-e0605n11:36": "85.",
  "anya-e0605n11:37": " Nānāsanāni sayanāni nivesanāni,",
  "anya-e0605n11:38": "Bhābhānibhāni ratanākarasannibhāni;",
  "anya-e0605n11:39": "Tatrussitāni ratanaddhajabhūsitāni,",
  "anya-e0605n11:40": "Hitvā va tāni himabindusamāni tāni.",
  "anya-e0605n11:41": "86.",
  "anya-e0605n11:42": " Nānāvidhehi ratanehi samujjalehi,",
  "anya-e0605n11:43": "Nārīhi niccamupagāyitahammiyehi;",
  "anya-e0605n11:44": "Rajjehi cakkaratanādivibhūsitehi,",
  "anya-e0605n11:45": "Yāto tato hi mahito purisassarehi."
}