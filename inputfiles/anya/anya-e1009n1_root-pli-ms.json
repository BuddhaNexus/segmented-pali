{
  "anya-e1009n1:0": " Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1009n1:1": "Naradakkhadīpanī",
  "anya-e1009n1:2": "Pālito yeva saddhammo,",
  "anya-e1009n1:3": "Pālitena varena ca;",
  "anya-e1009n1:4": "Pālite suṭṭhu yaṁ sīlaṁ,",
  "anya-e1009n1:5": "Pālitaṁ dhamma-sundaraṁ.",
  "anya-e1009n1:6": "Sammā āraddhaṁ sabba-sampattīnaṁ,",
  "anya-e1009n1:7": "Mūlaṁ hotīti daṭṭhabbaṁ.",
  "anya-e1009n1:8": "Vīriyārabbho bhikkhave,",
  "anya-e1009n1:9": "Mahato atthāya saṁvattatīti.",
  "anya-e1009n1:10": "Tasmā vīriyameva kattabbaṁ,",
  "anya-e1009n1:11": "Vīriyavato hi acintitaṁpi hoti.",
  "anya-e1009n1:12": "Yathā hi tacchakānaṁ suttaṁ,",
  "anya-e1009n1:13": "Pamāṇaṁ hoti;",
  "anya-e1009n1:14": "Eva metampi viññūnaṁ.",
  "anya-e1009n1:15": "Suvijāno bhavaṁ hoti,",
  "anya-e1009n1:16": "Dubbijāno parābhavo;",
  "anya-e1009n1:17": "Dhammakāmo bhavaṁ hoti,",
  "anya-e1009n1:18": "Dhammadessī parābhavo.",
  "anya-e1009n1:19": "Ayaṁ dhammatāti ayaṁ sabhāvo,",
  "anya-e1009n1:20": "Ayaṁ niyāmoti vuttaṁ hoti.",
  "anya-e1009n1:21": "Ye dhammā hetupabhavā,",
  "anya-e1009n1:22": "Tesaṁ hetuṁ tathāgato.",
  "anya-e1009n1:23": "Ratanattayaṁ , santataṁ, ahaṁ vandāmi;",
  "anya-e1009n1:24": "Ācariyaṁ, so ahaṁ, niccaṁ namāmi;",
  "anya-e1009n1:25": "‘‘Hotu sabbaṁ, maṅgalaṁ, mamaṁ sabbadhi’’.",
  "anya-e1009n1:26": "Naradakkhadīpanī",
  "anya-e1009n1:27": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1009n1:28": "Paṇāma"
}