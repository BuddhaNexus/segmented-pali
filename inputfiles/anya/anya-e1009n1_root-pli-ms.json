{
  "anya-e1009n1:0": " Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1009n1:1": "Naradakkhadīpanī",
  "anya-e1009n1:2": "Pālito yeva saddhammo,",
  "anya-e1009n1:3": "Pālitena varena ca;",
  "anya-e1009n1:4": "Pālite suṭṭhu yaṃ sīlaṃ,",
  "anya-e1009n1:5": "Pālitaṃ dhamma-sundaraṃ.",
  "anya-e1009n1:6": "Sammā āraddhaṃ sabba-sampattīnaṃ,",
  "anya-e1009n1:7": "Mūlaṃ hotīti daṭṭhabbaṃ.",
  "anya-e1009n1:8": "Vīriyārabbho bhikkhave,",
  "anya-e1009n1:9": "Mahato atthāya saṃvattatīti.",
  "anya-e1009n1:10": "Tasmā vīriyameva kattabbaṃ,",
  "anya-e1009n1:11": "Vīriyavato hi acintitaṃpi hoti.",
  "anya-e1009n1:12": "Yathā hi tacchakānaṃ suttaṃ,",
  "anya-e1009n1:13": "Pamāṇaṃ hoti;",
  "anya-e1009n1:14": "Eva metampi viññūnaṃ.",
  "anya-e1009n1:15": "Suvijāno bhavaṃ hoti,",
  "anya-e1009n1:16": "Dubbijāno parābhavo;",
  "anya-e1009n1:17": "Dhammakāmo bhavaṃ hoti,",
  "anya-e1009n1:18": "Dhammadessī parābhavo.",
  "anya-e1009n1:19": "Ayaṃ dhammatāti ayaṃ sabhāvo,",
  "anya-e1009n1:20": "Ayaṃ niyāmoti vuttaṃ hoti.",
  "anya-e1009n1:21": "Ye dhammā hetupabhavā,",
  "anya-e1009n1:22": "Tesaṃ hetuṃ tathāgato.",
  "anya-e1009n1:23": "Ratanattayaṃ , santataṃ, ahaṃ vandāmi;",
  "anya-e1009n1:24": "Ācariyaṃ, so ahaṃ, niccaṃ namāmi;",
  "anya-e1009n1:25": "‘‘Hotu sabbaṃ, maṅgalaṃ, mamaṃ sabbadhi’’.",
  "anya-e1009n1:26": "Naradakkhadīpanī",
  "anya-e1009n1:27": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1009n1:28": "Paṇāma"
}