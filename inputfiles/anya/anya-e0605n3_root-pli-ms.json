{
  "anya-e0605n3:0": "Anaññasādhāraṇadīpanīgāthā",
  "anya-e0605n3:1": "7.",
  "anya-e0605n3:2": " Abbhuggatā yassa guṇā anantā,",
  "anya-e0605n3:3": "Tibuddhakhettekadivākaroti;",
  "anya-e0605n3:4": "Jānāti so lokamimaṃ parañca,",
  "anya-e0605n3:5": "Sacetanañceva acetanañca;",
  "anya-e0605n3:6": "Sakassa santānagataṃ paresaṃ,",
  "anya-e0605n3:7": "Byatītamappattakamatrabhūtaṃ.",
  "anya-e0605n3:8": "8.",
  "anya-e0605n3:9": " Anantasattesu ca lokadhātusu,",
  "anya-e0605n3:10": "Ekova sabbepi samā na tena;",
  "anya-e0605n3:11": "Disāsu pubbādisu cakkavāḷā,",
  "anya-e0605n3:12": "Sahassasaṅkhāyapi appameyyā;",
  "anya-e0605n3:13": "Ye tesu devā manujā ca brahmā,",
  "anya-e0605n3:14": "Ekattha saṅgamma hi mantayantā.",
  "anya-e0605n3:15": "9.",
  "anya-e0605n3:16": " Anādikālāgatanāmarūpinaṃ,",
  "anya-e0605n3:17": "Yathāsakaṃ hetuphalattavuttinaṃ;",
  "anya-e0605n3:18": "Tabbhāvabhāvittamasambhuṇantā,",
  "anya-e0605n3:19": "Nānāvipallāsamanupaviṭṭhā.",
  "anya-e0605n3:20": "10.",
  "anya-e0605n3:21": " Kammappavattiñca phalappavattiṃ,",
  "anya-e0605n3:22": "Ekattanānattanirīhadhammataṃ;",
  "anya-e0605n3:23": "Viññattisantānaghanena channato,",
  "anya-e0605n3:24": "Sivañjasaṃ no bhaṇituṃ samatthā.",
  "anya-e0605n3:25": "11.",
  "anya-e0605n3:26": " Eko va so santikaro pabhaṅkaro,",
  "anya-e0605n3:27": "Saṅkhāya ñeyyāni asesitāni;",
  "anya-e0605n3:28": "Tesañhi majjhe paramāsambhīvadaṃ,",
  "anya-e0605n3:29": "Sivañjasaṃ dīpayituṃ samattho.",
  "anya-e0605n3:30": "12.",
  "anya-e0605n3:31": " So gotamo sakyasuto munindo,",
  "anya-e0605n3:32": "Sabbassa lokassa padīpabhūto;",
  "anya-e0605n3:33": "Anantasatte bhavabandhanamhā,",
  "anya-e0605n3:34": "Mocesi kāruññaphalānupekkhī."
}