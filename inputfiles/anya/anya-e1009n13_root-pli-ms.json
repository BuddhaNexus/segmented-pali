{
  "anya-e1009n13:0": "Naradakkha thomanā āsīsa",
  "anya-e1009n13:1": "3. Gāthā",
  "anya-e1009n13:2": "1.",
  "anya-e1009n13:3": " Pālito pāḷiyā cheko,",
  "anya-e1009n13:4": "Tvaṁsi gambhīra-ñāṇavā;",
  "anya-e1009n13:5": "Pāliyāva pālitassa,",
  "anya-e1009n13:6": "Dadāmidāni bho ahaṁ.",
  "anya-e1009n13:7": "2.",
  "anya-e1009n13:8": " Dakkhāvādesu kusalo,",
  "anya-e1009n13:9": "Pālito sāsanandharī;",
  "anya-e1009n13:10": "Piṭakesu ajjhogāyha,",
  "anya-e1009n13:11": "Naradakkhaṁbhisaṅkharī.",
  "anya-e1009n13:12": "3.",
  "anya-e1009n13:13": " Suta-dharena racitaṁ,",
  "anya-e1009n13:14": "Etaṁ sāra-gavesino;",
  "anya-e1009n13:15": "Atandikā su-dakkhantu,",
  "anya-e1009n13:16": "Aggagga-sāsane ratā.",
  "anya-e1009n13:17": "Naradakkha thomanā āsīsa",
  "anya-e1009n13:18": "2. Gāthā",
  "anya-e1009n13:19": "1.",
  "anya-e1009n13:20": " Ñuṁ pālitodha jāto yo,",
  "anya-e1009n13:21": "Thero so abbhutova ñuṁ;",
  "anya-e1009n13:22": "Ñuṁ mahā-pālito santo,",
  "anya-e1009n13:23": "Nikāya-pālito ca ñuṁ.",
  "anya-e1009n13:24": "2.",
  "anya-e1009n13:25": " Ñuṁ nara-dakkha-ganthaṁ yaṁ,",
  "anya-e1009n13:26": "Sovakā nara-dakkha-daṁ;",
  "anya-e1009n13:27": "Narā dakkhantu sammā ca,",
  "anya-e1009n13:28": "Dakkhattaṁ pāpuṇantu ñuṁ."
}