{
  "anya-e0702n0:0": " Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e0702n0:1": "Sāsanavaṁsappadīpikā",
  "anya-e0702n0:2": "Buddhaṁ sumālī dvipaduttamo tamo,",
  "anya-e0702n0:3": "Hantvāna bodhesidha paṅkajaṁ kajaṁ;",
  "anya-e0702n0:4": "Maggaggaselamhi suñaṭṭhito ṭhito,",
  "anya-e0702n0:5": "So maṁ ciraṁ pātu sukhaṁ sadā sadā.",
  "anya-e0702n0:6": "Sīhaḷaddīpatoyeva,",
  "anya-e0702n0:7": "Āgatehi disantare;",
  "anya-e0702n0:8": "Bhikkhūhi yācito kassaṁ,",
  "anya-e0702n0:9": "Sāsnavaṁsappadīpikaṁ.",
  "anya-e0702n0:10": "Kāmañca porāṇehi yā,",
  "anya-e0702n0:11": "Sāsnavaṁsappadipikā;",
  "anya-e0702n0:12": "Vitthāra vācanāmaggā,",
  "anya-e0702n0:13": "Viracitā vinicchayā.",
  "anya-e0702n0:14": "Sā pana mrammasāsāya,",
  "anya-e0702n0:15": "Katattāyeva etesaṁ;",
  "anya-e0702n0:16": "Dīpantaranivāsinaṁ,",
  "anya-e0702n0:17": "Vahāti suṭṭhunātthaṁ.",
  "anya-e0702n0:18": "Tasmā hi mūlabhāsāya,",
  "anya-e0702n0:19": "Karissāmi ahaṁ have;",
  "anya-e0702n0:20": "Saṁsanditvāna ganthehi,",
  "anya-e0702n0:21": "Taṁ sallakkhentu sādhavoti.",
  "anya-e0702n0:22": "Tatrāyaṁmātikā –",
  "anya-e0702n0:23": "1. Navaṭṭhānāgatasāsanavaṁsakathāmaggo,",
  "anya-e0702n0:24": "2. Sīhaḷadīpikasāsanavaṁsakathāmaggo,",
  "anya-e0702n0:25": "3. Suvaṇṇabhūmisāsanavaṁsakathāmaggo,",
  "anya-e0702n0:26": "4. Yonakaraṭṭhasāsanavaṁsakathāmaggo,",
  "anya-e0702n0:27": "5. Vanavāsīraṭṭhasāsanavaṁsakathāmaggo,",
  "anya-e0702n0:28": "6. Aparantaraṭṭhasāsanavaṁsakathāmaggo,",
  "anya-e0702n0:29": "7. Kasmīragandhāraraṭṭhasāsanavaṁsakathāmaggo,",
  "anya-e0702n0:30": "8. Mahiṁsakaraṭṭhasāsanavaṁsakathāmaggo,",
  "anya-e0702n0:31": "9. Mahāraṭṭhasāsanavaṁsavathāmaggo,",
  "anya-e0702n0:32": "10. Cinaraṭṭhasāsanavaṁsakathāmaggocāti."
}