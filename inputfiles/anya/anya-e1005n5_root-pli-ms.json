{
  "anya-e1005n5:0": "6. Rājakaṇḍo",
  "anya-e1005n5:1": "112.",
  "anya-e1005n5:2": " Ekayāmaṁ saye rājā,",
  "anya-e1005n5:3": "Dviyāmaññeva paṇḍito;",
  "anya-e1005n5:4": "Gharāvāso tiyāmaṁva,",
  "anya-e1005n5:5": "Catuyāmaṁ tu yācako.",
  "anya-e1005n5:6": "113.",
  "anya-e1005n5:7": " Dhanavā sutavā rājā,",
  "anya-e1005n5:8": "Nadī vejjo cimepañca;",
  "anya-e1005n5:9": "Yattha dese na vijjanti,",
  "anya-e1005n5:10": "Na tattha divasaṁ vase.",
  "anya-e1005n5:11": "114.",
  "anya-e1005n5:12": " Yasmiṁ padese na māno,",
  "anya-e1005n5:13": "Na pemaṁ naca bandhavā;",
  "anya-e1005n5:14": "Naca vijjāgamo koci,",
  "anya-e1005n5:15": "Na tattha divasaṁ vase.",
  "anya-e1005n5:16": "115.",
  "anya-e1005n5:17": " Aputtakaṁ gharaṁ suññaṁ,",
  "anya-e1005n5:18": "Raṭṭhaṁ suññaṁ arājakaṁ;",
  "anya-e1005n5:19": "A sippassa mukhaṁ suññaṁ,",
  "anya-e1005n5:20": "Sabba suññaṁ daliddakā.",
  "anya-e1005n5:21": "116.",
  "anya-e1005n5:22": " Dhanamiccheyya vāṇijjo,",
  "anya-e1005n5:23": "Vijjamicche bhajesutaṁ;",
  "anya-e1005n5:24": "Puttamicche taruṇitthiṁ,",
  "anya-e1005n5:25": "Rājāmaccaṁ vasaṁ game.",
  "anya-e1005n5:26": "117.",
  "anya-e1005n5:27": " Naṭṭhoyati asantuṭṭho,",
  "anya-e1005n5:28": "Santuṭṭho ca mahīpati;",
  "anya-e1005n5:29": "Lajjā ca gaṇikā naṭṭhā,",
  "anya-e1005n5:30": "Nillajjā kuladhītikā.",
  "anya-e1005n5:31": "118.",
  "anya-e1005n5:32": " Pakkhīnaṁ balamākāso,",
  "anya-e1005n5:33": "Macchānamudakaṁ balaṁ;",
  "anya-e1005n5:34": "Dubbalassa balaṁ rājā,",
  "anya-e1005n5:35": "Kumārānaṁ rudaṁ balaṁ.",
  "anya-e1005n5:36": "119.",
  "anya-e1005n5:37": " Khamā jāgariyuṭṭhānaṁ,",
  "anya-e1005n5:38": "Saṁvibhāgo dayikkhaṇā;",
  "anya-e1005n5:39": "Nāyakassa guṇā ete,",
  "anya-e1005n5:40": "Icchitabbā sataṁ guṇā.",
  "anya-e1005n5:41": "120.",
  "anya-e1005n5:42": " Sakiṁ vadanti rājāno,",
  "anya-e1005n5:43": "Sakiṁ samaṇabrāhmaṇā;",
  "anya-e1005n5:44": "Sakiṁ sappurisā loke,",
  "anya-e1005n5:45": "Esa dhammo sanantano.",
  "anya-e1005n5:46": "121.",
  "anya-e1005n5:47": " Alaso gihī kāmabhogī na sādhu,",
  "anya-e1005n5:48": "Asaññato pabbajito na sādhu;",
  "anya-e1005n5:49": "Rājā anisammakārī na sādhu,",
  "anya-e1005n5:50": "Paṇḍito kodhano taṁpi na sādhu.",
  "anya-e1005n5:51": "122.",
  "anya-e1005n5:52": " Bahavo yattha nettāro,",
  "anya-e1005n5:53": "Sabbe paṇḍitamānino;",
  "anya-e1005n5:54": "Sabbe mahattamicchanti,",
  "anya-e1005n5:55": "Tesaṁ kammaṁ vinassati.",
  "anya-e1005n5:56": "123.",
  "anya-e1005n5:57": " Āyaṁ khayaṁ sayaṁ jaññā,",
  "anya-e1005n5:58": "Rājā sayaṁ katākataṁ;",
  "anya-e1005n5:59": "Niggahe niggahetabbaṁ,",
  "anya-e1005n5:60": "Paggahe paggahārahaṁ.",
  "anya-e1005n5:61": "124.",
  "anya-e1005n5:62": " Piṭṭhitokkaṁ niseveyya,",
  "anya-e1005n5:63": "Kucchināva hutāsanaṁ;",
  "anya-e1005n5:64": "Sāmikaṁ sabbabhāgena,",
  "anya-e1005n5:65": "Paralokaṁ amohavā.",
  "anya-e1005n5:66": "125.",
  "anya-e1005n5:67": " Aggi āpo itthimūḷho,",
  "anya-e1005n5:68": "Sappo rāja-kulānica;",
  "anya-e1005n5:69": "Apayantena gantabbā,",
  "anya-e1005n5:70": "Acceka-pāṇahārakā.",
  "anya-e1005n5:71": "126.",
  "anya-e1005n5:72": " Paduṭṭha-bhariya-saṁvāso,",
  "anya-e1005n5:73": "Paduṭṭha citta dāsako;",
  "anya-e1005n5:74": "Sa-sappe ca ghare vāso,",
  "anya-e1005n5:75": "Maccu eva na saṁsayo.",
  "anya-e1005n5:76": "127.",
  "anya-e1005n5:77": " Mūḷha sisso padesena,",
  "anya-e1005n5:78": "Kunārī bharaṇena ca;",
  "anya-e1005n5:79": "Asatā sampayogena,",
  "anya-e1005n5:80": "Paṇḍitoppavasīdati.",
  "anya-e1005n5:81": "128.",
  "anya-e1005n5:82": " Mātā puttakaraṁ pāpaṁ,",
  "anya-e1005n5:83": "Sissapāpaṁ gurukatā;",
  "anya-e1005n5:84": "Rājā raṭṭhakaraṁ pāpaṁ,",
  "anya-e1005n5:85": "Rājapāpaṁ purohito.",
  "anya-e1005n5:86": "129.",
  "anya-e1005n5:87": " Akodhena jine kodhaṁ,",
  "anya-e1005n5:88": "Asādhuṁ sādhunā jine;",
  "anya-e1005n5:89": "Jine macchariṁ dānena,",
  "anya-e1005n5:90": "Saccenālīkavādinaṁ;",
  "anya-e1005n5:91": "130.",
  "anya-e1005n5:92": " Adantaṁ damanaṁ dānaṁ,",
  "anya-e1005n5:93": "Dānaṁ sabbattha sādhakaṁ;",
  "anya-e1005n5:94": "Dānena piya vācāya,",
  "anya-e1005n5:95": "Unnamanti namanti ca;",
  "anya-e1005n5:96": "131.",
  "anya-e1005n5:97": " Dānaṁ sinehabhesajjaṁ,",
  "anya-e1005n5:98": "Maccheraṁ dosanosadhaṁ;",
  "anya-e1005n5:99": "Dānaṁ yasassī bhesajjaṁ,",
  "anya-e1005n5:100": "Maccheraṁ kapaṇosadhaṁ.",
  "anya-e1005n5:101": "132.",
  "anya-e1005n5:102": " Bahūnamappasārānaṁ,",
  "anya-e1005n5:103": "Sāmaggiyā jayaṁ jaye;",
  "anya-e1005n5:104": "Tiṇehi vattate yottaṁ,",
  "anya-e1005n5:105": "Tena nāgopi bajjhate.",
  "anya-e1005n5:106": "133.",
  "anya-e1005n5:107": " Sahāyo asamatthopi,",
  "anya-e1005n5:108": "Tejasā kiṁkarissati;",
  "anya-e1005n5:109": "Nivāte jalito aggi,",
  "anya-e1005n5:110": "Sayame vūpasampati.",
  "anya-e1005n5:111": "134.",
  "anya-e1005n5:112": " Na raññā samakaṁ bhuñje,",
  "anya-e1005n5:113": "Kāmabhogaṁ kudācanaṁ;",
  "anya-e1005n5:114": "Ākappaṁ rasa bhuttiṁvā,",
  "anya-e1005n5:115": "Mālā gandha vilepanaṁ;",
  "anya-e1005n5:116": "Vatthaṁ sabbaalaṅkāraṁ,",
  "anya-e1005n5:117": "Na raññā sadisaṁ kare.",
  "anya-e1005n5:118": "135.",
  "anya-e1005n5:119": " Na me rājā sakhā hoti,",
  "anya-e1005n5:120": "Na rājā hoti methuno;",
  "anya-e1005n5:121": "Eso sāmiko mayhanti,",
  "anya-e1005n5:122": "Citte niṭṭhaṁ suthāpaye.",
  "anya-e1005n5:123": "136.",
  "anya-e1005n5:124": " Nātidūre bhaje rañño,",
  "anya-e1005n5:125": "Nāccāsannopavātake;",
  "anya-e1005n5:126": "Ujuke nātininne ca,",
  "anya-e1005n5:127": "Na bhaje uccamāsane.",
  "anya-e1005n5:128": "Chadose vajje sevako,",
  "anya-e1005n5:129": "Tiṭṭhe aggiṁva saṁyato.",
  "anya-e1005n5:130": "137.",
  "anya-e1005n5:131": " Guṇī sabbaññu tulyopi,",
  "anya-e1005n5:132": "Nasobhati anissayo;",
  "anya-e1005n5:133": "Anagghamopi maṇiseṭṭho,",
  "anya-e1005n5:134": "Hemaṁ nissāya sobhati.",
  "anya-e1005n5:135": "Rājakaṇḍo niṭṭhito."
}