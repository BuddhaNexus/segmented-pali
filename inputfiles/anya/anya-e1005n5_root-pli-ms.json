{
  "anya-e1005n5:0": "6. Rājakaṇḍo",
  "anya-e1005n5:1": "112.",
  "anya-e1005n5:2": " Ekayāmaṃ saye rājā,",
  "anya-e1005n5:3": "Dviyāmaññeva paṇḍito;",
  "anya-e1005n5:4": "Gharāvāso tiyāmaṃva,",
  "anya-e1005n5:5": "Catuyāmaṃ tu yācako.",
  "anya-e1005n5:6": "113.",
  "anya-e1005n5:7": " Dhanavā sutavā rājā,",
  "anya-e1005n5:8": "Nadī vejjo cimepañca;",
  "anya-e1005n5:9": "Yattha dese na vijjanti,",
  "anya-e1005n5:10": "Na tattha divasaṃ vase.",
  "anya-e1005n5:11": "114.",
  "anya-e1005n5:12": " Yasmiṃ padese na māno,",
  "anya-e1005n5:13": "Na pemaṃ naca bandhavā;",
  "anya-e1005n5:14": "Naca vijjāgamo koci,",
  "anya-e1005n5:15": "Na tattha divasaṃ vase.",
  "anya-e1005n5:16": "115.",
  "anya-e1005n5:17": " Aputtakaṃ gharaṃ suññaṃ,",
  "anya-e1005n5:18": "Raṭṭhaṃ suññaṃ arājakaṃ;",
  "anya-e1005n5:19": "A sippassa mukhaṃ suññaṃ,",
  "anya-e1005n5:20": "Sabba suññaṃ daliddakā.",
  "anya-e1005n5:21": "116.",
  "anya-e1005n5:22": " Dhanamiccheyya vāṇijjo,",
  "anya-e1005n5:23": "Vijjamicche bhajesutaṃ;",
  "anya-e1005n5:24": "Puttamicche taruṇitthiṃ,",
  "anya-e1005n5:25": "Rājāmaccaṃ vasaṃ game.",
  "anya-e1005n5:26": "117.",
  "anya-e1005n5:27": " Naṭṭhoyati asantuṭṭho,",
  "anya-e1005n5:28": "Santuṭṭho ca mahīpati;",
  "anya-e1005n5:29": "Lajjā ca gaṇikā naṭṭhā,",
  "anya-e1005n5:30": "Nillajjā kuladhītikā.",
  "anya-e1005n5:31": "118.",
  "anya-e1005n5:32": " Pakkhīnaṃ balamākāso,",
  "anya-e1005n5:33": "Macchānamudakaṃ balaṃ;",
  "anya-e1005n5:34": "Dubbalassa balaṃ rājā,",
  "anya-e1005n5:35": "Kumārānaṃ rudaṃ balaṃ.",
  "anya-e1005n5:36": "119.",
  "anya-e1005n5:37": " Khamā jāgariyuṭṭhānaṃ,",
  "anya-e1005n5:38": "Saṃvibhāgo dayikkhaṇā;",
  "anya-e1005n5:39": "Nāyakassa guṇā ete,",
  "anya-e1005n5:40": "Icchitabbā sataṃ guṇā.",
  "anya-e1005n5:41": "120.",
  "anya-e1005n5:42": " Sakiṃ vadanti rājāno,",
  "anya-e1005n5:43": "Sakiṃ samaṇabrāhmaṇā;",
  "anya-e1005n5:44": "Sakiṃ sappurisā loke,",
  "anya-e1005n5:45": "Esa dhammo sanantano.",
  "anya-e1005n5:46": "121.",
  "anya-e1005n5:47": " Alaso gihī kāmabhogī na sādhu,",
  "anya-e1005n5:48": "Asaññato pabbajito na sādhu;",
  "anya-e1005n5:49": "Rājā anisammakārī na sādhu,",
  "anya-e1005n5:50": "Paṇḍito kodhano taṃpi na sādhu.",
  "anya-e1005n5:51": "122.",
  "anya-e1005n5:52": " Bahavo yattha nettāro,",
  "anya-e1005n5:53": "Sabbe paṇḍitamānino;",
  "anya-e1005n5:54": "Sabbe mahattamicchanti,",
  "anya-e1005n5:55": "Tesaṃ kammaṃ vinassati.",
  "anya-e1005n5:56": "123.",
  "anya-e1005n5:57": " Āyaṃ khayaṃ sayaṃ jaññā,",
  "anya-e1005n5:58": "Rājā sayaṃ katākataṃ;",
  "anya-e1005n5:59": "Niggahe niggahetabbaṃ,",
  "anya-e1005n5:60": "Paggahe paggahārahaṃ.",
  "anya-e1005n5:61": "124.",
  "anya-e1005n5:62": " Piṭṭhitokkaṃ niseveyya,",
  "anya-e1005n5:63": "Kucchināva hutāsanaṃ;",
  "anya-e1005n5:64": "Sāmikaṃ sabbabhāgena,",
  "anya-e1005n5:65": "Paralokaṃ amohavā.",
  "anya-e1005n5:66": "125.",
  "anya-e1005n5:67": " Aggi āpo itthimūḷho,",
  "anya-e1005n5:68": "Sappo rāja-kulānica;",
  "anya-e1005n5:69": "Apayantena gantabbā,",
  "anya-e1005n5:70": "Acceka-pāṇahārakā.",
  "anya-e1005n5:71": "126.",
  "anya-e1005n5:72": " Paduṭṭha-bhariya-saṃvāso,",
  "anya-e1005n5:73": "Paduṭṭha citta dāsako;",
  "anya-e1005n5:74": "Sa-sappe ca ghare vāso,",
  "anya-e1005n5:75": "Maccu eva na saṃsayo.",
  "anya-e1005n5:76": "127.",
  "anya-e1005n5:77": " Mūḷha sisso padesena,",
  "anya-e1005n5:78": "Kunārī bharaṇena ca;",
  "anya-e1005n5:79": "Asatā sampayogena,",
  "anya-e1005n5:80": "Paṇḍitoppavasīdati.",
  "anya-e1005n5:81": "128.",
  "anya-e1005n5:82": " Mātā puttakaraṃ pāpaṃ,",
  "anya-e1005n5:83": "Sissapāpaṃ gurukatā;",
  "anya-e1005n5:84": "Rājā raṭṭhakaraṃ pāpaṃ,",
  "anya-e1005n5:85": "Rājapāpaṃ purohito.",
  "anya-e1005n5:86": "129.",
  "anya-e1005n5:87": " Akodhena jine kodhaṃ,",
  "anya-e1005n5:88": "Asādhuṃ sādhunā jine;",
  "anya-e1005n5:89": "Jine macchariṃ dānena,",
  "anya-e1005n5:90": "Saccenālīkavādinaṃ;",
  "anya-e1005n5:91": "130.",
  "anya-e1005n5:92": " Adantaṃ damanaṃ dānaṃ,",
  "anya-e1005n5:93": "Dānaṃ sabbattha sādhakaṃ;",
  "anya-e1005n5:94": "Dānena piya vācāya,",
  "anya-e1005n5:95": "Unnamanti namanti ca;",
  "anya-e1005n5:96": "131.",
  "anya-e1005n5:97": " Dānaṃ sinehabhesajjaṃ,",
  "anya-e1005n5:98": "Maccheraṃ dosanosadhaṃ;",
  "anya-e1005n5:99": "Dānaṃ yasassī bhesajjaṃ,",
  "anya-e1005n5:100": "Maccheraṃ kapaṇosadhaṃ.",
  "anya-e1005n5:101": "132.",
  "anya-e1005n5:102": " Bahūnamappasārānaṃ,",
  "anya-e1005n5:103": "Sāmaggiyā jayaṃ jaye;",
  "anya-e1005n5:104": "Tiṇehi vattate yottaṃ,",
  "anya-e1005n5:105": "Tena nāgopi bajjhate.",
  "anya-e1005n5:106": "133.",
  "anya-e1005n5:107": " Sahāyo asamatthopi,",
  "anya-e1005n5:108": "Tejasā kiṃkarissati;",
  "anya-e1005n5:109": "Nivāte jalito aggi,",
  "anya-e1005n5:110": "Sayame vūpasampati.",
  "anya-e1005n5:111": "134.",
  "anya-e1005n5:112": " Na raññā samakaṃ bhuñje,",
  "anya-e1005n5:113": "Kāmabhogaṃ kudācanaṃ;",
  "anya-e1005n5:114": "Ākappaṃ rasa bhuttiṃvā,",
  "anya-e1005n5:115": "Mālā gandha vilepanaṃ;",
  "anya-e1005n5:116": "Vatthaṃ sabbaalaṅkāraṃ,",
  "anya-e1005n5:117": "Na raññā sadisaṃ kare.",
  "anya-e1005n5:118": "135.",
  "anya-e1005n5:119": " Na me rājā sakhā hoti,",
  "anya-e1005n5:120": "Na rājā hoti methuno;",
  "anya-e1005n5:121": "Eso sāmiko mayhanti,",
  "anya-e1005n5:122": "Citte niṭṭhaṃ suthāpaye.",
  "anya-e1005n5:123": "136.",
  "anya-e1005n5:124": " Nātidūre bhaje rañño,",
  "anya-e1005n5:125": "Nāccāsannopavātake;",
  "anya-e1005n5:126": "Ujuke nātininne ca,",
  "anya-e1005n5:127": "Na bhaje uccamāsane.",
  "anya-e1005n5:128": "Chadose vajje sevako,",
  "anya-e1005n5:129": "Tiṭṭhe aggiṃva saṃyato.",
  "anya-e1005n5:130": "137.",
  "anya-e1005n5:131": " Guṇī sabbaññu tulyopi,",
  "anya-e1005n5:132": "Nasobhati anissayo;",
  "anya-e1005n5:133": "Anagghamopi maṇiseṭṭho,",
  "anya-e1005n5:134": "Hemaṃ nissāya sobhati.",
  "anya-e1005n5:135": "Rājakaṇḍo niṭṭhito."
}