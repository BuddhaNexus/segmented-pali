{
  "anya-e1004n3:0": "Nāyakakathā",
  "anya-e1004n3:1": "164.",
  "anya-e1004n3:2": " Kassako vāṇijomacco,",
  "anya-e1004n3:3": "Samaṇo sutasīlavā;",
  "anya-e1004n3:4": "Tesu vipulajātesu,",
  "anya-e1004n3:5": "Raṭṭhaṃpi vipulaṃ siyā.",
  "anya-e1004n3:6": "165.",
  "anya-e1004n3:7": " Tesu dubbalajātesu,",
  "anya-e1004n3:8": "Raṭṭhaṃpi dubbalaṃ siyā;",
  "anya-e1004n3:9": "Tasmā raṭṭhaṃpi vipulaṃ,",
  "anya-e1004n3:10": "Dhāraye raṭṭhasāravā.",
  "anya-e1004n3:11": "166.",
  "anya-e1004n3:12": " Mahārukkhassa phalino,",
  "anya-e1004n3:13": "Āmaṃ chindati yo phalaṃ;",
  "anya-e1004n3:14": "Rasañcassa najānāti,",
  "anya-e1004n3:15": "Bījañcassa vinassati.",
  "anya-e1004n3:16": "167.",
  "anya-e1004n3:17": " Mahārukkhupamaṃ raṭṭhaṃ,",
  "anya-e1004n3:18": "Yo adhammena sāsati;",
  "anya-e1004n3:19": "Rasañcassa najānāti,",
  "anya-e1004n3:20": "Raṭṭhañcāpi vinassati.",
  "anya-e1004n3:21": "168.",
  "anya-e1004n3:22": " Mahārukkhassa phalino,",
  "anya-e1004n3:23": "Pakkaṃ chindati yo phalaṃ;",
  "anya-e1004n3:24": "Rasañcassa vijānāti,",
  "anya-e1004n3:25": "Bījañcassa nanassati.",
  "anya-e1004n3:26": "169.",
  "anya-e1004n3:27": " Mahārukkhupamaṃ raṭṭhaṃ,",
  "anya-e1004n3:28": "Dhammena yo pasāsati;",
  "anya-e1004n3:29": "Rasañcassa vijānāti,",
  "anya-e1004n3:30": "Raṭṭhañcāpi nanassati.",
  "anya-e1004n3:31": "170.",
  "anya-e1004n3:32": " Janappadañca yorājā,",
  "anya-e1004n3:33": "Adhammena pasāsati;",
  "anya-e1004n3:34": "Sabbo sadhihi sorājā,",
  "anya-e1004n3:35": "Viruddho hoti khattiyo.",
  "anya-e1004n3:36": "171.",
  "anya-e1004n3:37": " Tatheva nigame hiṃsaṃ,",
  "anya-e1004n3:38": "Ye yuttā kayavikkaye;",
  "anya-e1004n3:39": "Suṅkadānabalīkāre,",
  "anya-e1004n3:40": "Sa kosena virujjhati.",
  "anya-e1004n3:41": "172.",
  "anya-e1004n3:42": " Mahāravarakhettesu,",
  "anya-e1004n3:43": "Saṅga, me katanissame;",
  "anya-e1004n3:44": "Ussite hiṃsayaṃ rājā,",
  "anya-e1004n3:45": "Sabalena virujjhati.",
  "anya-e1004n3:46": "173.",
  "anya-e1004n3:47": " Tatheva isayo hiṃso,",
  "anya-e1004n3:48": "Saṃyame brahmacāriyo;",
  "anya-e1004n3:49": "Adhammacāri khattīyo,",
  "anya-e1004n3:50": "So saggena virujjhati.",
  "anya-e1004n3:51": "174.",
  "anya-e1004n3:52": " Sayaṃkatā naparena,",
  "anya-e1004n3:53": "Mahānajjo juvakatā;",
  "anya-e1004n3:54": "Issarena tathā raññā,",
  "anya-e1004n3:55": "Saraṭṭhe adhipaccatā.",
  "anya-e1004n3:56": "175.",
  "anya-e1004n3:57": " Pāpaṃvāpi hi puññaṃvā,",
  "anya-e1004n3:58": "Padhāno yaṃ karotice;",
  "anya-e1004n3:59": "Lokopevaṃ karotyeva,",
  "anya-e1004n3:60": "Taṃ vijāneyya paṇḍito.",
  "anya-e1004n3:61": "176.",
  "anya-e1004n3:62": " Kacchapīnañca macchīnaṃ,",
  "anya-e1004n3:63": "Kukkuṭīnañca dhenūnaṃ;",
  "anya-e1004n3:64": "Puttaposā yathāhoti,",
  "anya-e1004n3:65": "Tathā maccesu rājūnaṃ.",
  "anya-e1004n3:66": "177.",
  "anya-e1004n3:67": " Anāyakā vinassanti,",
  "anya-e1004n3:68": "Nassanti bahunāyakā;",
  "anya-e1004n3:69": "Thīnāyakā vinassanti,",
  "anya-e1004n3:70": "Nassanti susunāyakā.",
  "anya-e1004n3:71": "178.",
  "anya-e1004n3:72": " Bahuvo yattha netāro,",
  "anya-e1004n3:73": "Sabbe paṇḍitamānino;",
  "anya-e1004n3:74": "Sabbe mahattamicchanti,",
  "anya-e1004n3:75": "Kataṃ nesaṃ vinassati.",
  "anya-e1004n3:76": "179.",
  "anya-e1004n3:77": " Nodayāya vināsāya,",
  "anya-e1004n3:78": "Bahunāyakatā bhūsaṃ;",
  "anya-e1004n3:79": "Nomilanti vinassanti,",
  "anya-e1004n3:80": "Padmā nyakkehisattahi.",
  "anya-e1004n3:81": "180.",
  "anya-e1004n3:82": " Asantuṭṭho yatī naṭṭho,",
  "anya-e1004n3:83": "Santuṭṭhoca mahīpati;",
  "anya-e1004n3:84": "Salajjā gaṇikā naṭṭhā,",
  "anya-e1004n3:85": "Nilajjātu kulaṅganā.",
  "anya-e1004n3:86": "181.",
  "anya-e1004n3:87": " Na gaṇassa ggato gacche,",
  "anya-e1004n3:88": "Siddhe kamme samaṃphalaṃ;",
  "anya-e1004n3:89": "Kammavippatti cehoti,",
  "anya-e1004n3:90": "Mukharo tatra haññate.",
  "anya-e1004n3:91": "182.",
  "anya-e1004n3:92": " Padhiro ca tapassīni,",
  "anya-e1004n3:93": "Sūro raṇavaṇo tathā;",
  "anya-e1004n3:94": "Majjapo patithī rājā,",
  "anya-e1004n3:95": "Ete nasaddahāmahaṃ.",
  "anya-e1004n3:96": "183.",
  "anya-e1004n3:97": " Jāneyya pesane bhaccaṃ,",
  "anya-e1004n3:98": "Bandhavāpi bhayāgate;",
  "anya-e1004n3:99": "Āpadāsu tathā mittaṃ,",
  "anya-e1004n3:100": "Dārañca vibhavakkhaye.",
  "anya-e1004n3:101": "184.",
  "anya-e1004n3:102": " Raṇā paccāgataṃ sūraṃ,",
  "anya-e1004n3:103": "Dhanañca gharamāgataṃ;",
  "anya-e1004n3:104": "Jiṇṇa mannaṃ pasaṃseyya,",
  "anya-e1004n3:105": "Dārañca gatayobbanaṃ.",
  "anya-e1004n3:106": "185.",
  "anya-e1004n3:107": " Saddhāpemesu santesu,",
  "anya-e1004n3:108": "Nagaṇe māsakaṃ sataṃ;",
  "anya-e1004n3:109": "Saddhāvemesva santesu,",
  "anya-e1004n3:110": "Māsakaṃpi sataṃ gaṇe.",
  "anya-e1004n3:111": "186.",
  "anya-e1004n3:112": " Adantadamanaṃ dānaṃ,",
  "anya-e1004n3:113": "Dānaṃ sabbatthasādhakaṃ;",
  "anya-e1004n3:114": "Dānena piyavācena,",
  "anya-e1004n3:115": "Unnamanti namantica.",
  "anya-e1004n3:116": "187.",
  "anya-e1004n3:117": " Dānaṃ sinehabhesajjaṃ,",
  "anya-e1004n3:118": "Maccheraṃ dussanosadhaṃ;",
  "anya-e1004n3:119": "Dānaṃ yasassabhosajjaṃ,",
  "anya-e1004n3:120": "Maccheraṃ kappanosadhaṃ.",
  "anya-e1004n3:121": "188.",
  "anya-e1004n3:122": " Dubbhikkhe annadātaṃca,",
  "anya-e1004n3:123": "Subhikkheca hiraññadaṃ;",
  "anya-e1004n3:124": "Bhayecābhayatādānaṃ,",
  "anya-e1004n3:125": "Saggepi bahu maññate.",
  "anya-e1004n3:126": "189.",
  "anya-e1004n3:127": " Sataṃcakkhu sataṃkaṇṇā,",
  "anya-e1004n3:128": "Nāyakassa sutā sadā;",
  "anya-e1004n3:129": "Tathāpi andhapadhīro;",
  "anya-e1004n3:130": "Esā nāyakadhammatā.",
  "anya-e1004n3:131": "190.",
  "anya-e1004n3:132": " Khamā jāgariyu ṭṭhānaṃ,",
  "anya-e1004n3:133": "Saṃvibhāgo dayikkhaṇā;",
  "anya-e1004n3:134": "Nāyakassa guṇāete,",
  "anya-e1004n3:135": "Icchitabbā hitatthinā.",
  "anya-e1004n3:136": "191.",
  "anya-e1004n3:137": " Paribhūto mudū hoti,",
  "anya-e1004n3:138": "Atitikkhoca veravā;",
  "anya-e1004n3:139": "Etañcaubhayaṃ ñatvā,",
  "anya-e1004n3:140": "Anumajjhaṃ samācare.",
  "anya-e1004n3:141": "192.",
  "anya-e1004n3:142": " Nekantamudunā sakkā,",
  "anya-e1004n3:143": "Ekantatikhiṇenavā;",
  "anya-e1004n3:144": "Attaṃ mahante ṭhapetuṃ,",
  "anya-e1004n3:145": "Tasmā ubhaya mācare.",
  "anya-e1004n3:146": "193.",
  "anya-e1004n3:147": " Piṭṭhito kkaṃ niseveyya,",
  "anya-e1004n3:148": "Kucchinātu hutāsanaṃ;",
  "anya-e1004n3:149": "Sāmikaṃ sabbabhāvena,",
  "anya-e1004n3:150": "Paralokaṃ amāyāya.",
  "anya-e1004n3:151": "194.",
  "anya-e1004n3:152": " Naseve pharusaṃsāmiṃ,",
  "anya-e1004n3:153": "Taṃpi sevena macchariṃ;",
  "anya-e1004n3:154": "Tato paggaṇhakaṃseve,",
  "anya-e1004n3:155": "Seve niggaṇhakaṃ tato;",
  "anya-e1004n3:156": "195.",
  "anya-e1004n3:157": " Na sā rājā yo ajeyyaṃ jināti,",
  "anya-e1004n3:158": "Na so sakhāyo sakhāraṃ jināti;",
  "anya-e1004n3:159": "Na sā bhariyā patino virodhati,",
  "anya-e1004n3:160": "Na te vuttā ye nabharanti jiṇṇakaṃ.",
  "anya-e1004n3:161": "196.",
  "anya-e1004n3:162": " Na sā sabhā yattha nasanti santo,",
  "anya-e1004n3:163": "Na te santo ye navadanti dhammaṃ;",
  "anya-e1004n3:164": "Rāgañca dosañca pahāya mohaṃ,",
  "anya-e1004n3:165": "Dhammaṃ bhaṇantāva bhavanti santo.",
  "anya-e1004n3:166": "197.",
  "anya-e1004n3:167": " Sutassa rakkhā satatābhiyogo;",
  "anya-e1004n3:168": "Kulassa vattaṃ purisassa vijjā;",
  "anya-e1004n3:169": "Rañño pamādo pasamodhanassa,",
  "anya-e1004n3:170": "Thīnantu jānāmi na jātu rakkhaṃ."
}