{
  "anya-e1007n1:0": "Sūrassatīnīti",
  "anya-e1007n1:1": "1.",
  "anya-e1007n1:2": " Kataññutā ca saccañca,",
  "anya-e1007n1:3": "Lokasārā hi te duve;",
  "anya-e1007n1:4": "Lokāpi tehi tiṭṭhanti,",
  "anya-e1007n1:5": "Raṭṭhaṃ akaṃsu issaraṃ.",
  "anya-e1007n1:6": "2.",
  "anya-e1007n1:7": " Kākāca dujjanā loke,",
  "anya-e1007n1:8": "Malībhūtāva sabbadā;",
  "anya-e1007n1:9": "Iṭṭhaṃ guṇaṃ nāsayanti,",
  "anya-e1007n1:10": "Te ve lokassa verino.",
  "anya-e1007n1:11": "Migānaṃ siṅgālo anto,",
  "anya-e1007n1:12": "Pakkhīnaṃ pana vāyaso.",
  "anya-e1007n1:13": "Akāraṇaverī honti,",
  "anya-e1007n1:14": "Macchānaṃ dhīvarā yathā;",
  "anya-e1007n1:15": "Guṇīnaṃ sajjanānañca,",
  "anya-e1007n1:16": "Dujjanā niccaverino.",
  "anya-e1007n1:17": "3.",
  "anya-e1007n1:18": " Siriṃ bhonto suposetha,",
  "anya-e1007n1:19": "Siri mūlā hi sampadā;",
  "anya-e1007n1:20": "Siriya idha jotanti,",
  "anya-e1007n1:21": "Sirī sā sabbasiddhikā.",
  "anya-e1007n1:22": "4.",
  "anya-e1007n1:23": " Musā tamokarā loke,",
  "anya-e1007n1:24": "Saccaṃ mīditikārakaṃ;",
  "anya-e1007n1:25": "Musātamena dukkhanti,",
  "anya-e1007n1:26": "Saccābhāya sukhantive.",
  "anya-e1007n1:27": "Suvijānaṃ siṅgālānaṃ,",
  "anya-e1007n1:28": "Sakuṇānañca vassitaṃ;",
  "anya-e1007n1:29": "Manussavassitaṃ rāja,",
  "anya-e1007n1:30": "Dubbijānataraṃ tato.",
  "anya-e1007n1:31": "5.",
  "anya-e1007n1:32": " Kappaggisadisā issā,",
  "anya-e1007n1:33": "Jhāpeti sabbasampadaṃ;",
  "anya-e1007n1:34": "Muditā kappameghova,",
  "anya-e1007n1:35": "Ropeti sabbasampadaṃ.",
  "anya-e1007n1:36": "6.",
  "anya-e1007n1:37": " Yathā asanthirā thambhā,",
  "anya-e1007n1:38": "Thusarāsimhi ussitā;",
  "anya-e1007n1:39": "Tatheva kapicittānaṃ,",
  "anya-e1007n1:40": "Kammantā cañcalaṅgatā.",
  "anya-e1007n1:41": "Haliddirāgaṃ kapicittaṃ;",
  "anya-e1007n1:42": "Purisaṃ rāgavirāginaṃ,",
  "anya-e1007n1:43": "Edisaṃ tāta māsevi;",
  "anya-e1007n1:44": "Nimmanussampi ce siyā.",
  "anya-e1007n1:45": "7. [Ka]",
  "anya-e1007n1:46": " Pupphacumbi cittapattī,",
  "anya-e1007n1:47": "Sakavaṇṇena majjito;",
  "anya-e1007n1:48": "Vikkamī pupphato pupphaṃ,",
  "anya-e1007n1:49": "Nassāgāraṃ na saṅgamo.",
  "anya-e1007n1:50": "[Kha]",
  "anya-e1007n1:51": "Kippilikā dubbaṇṇāpi,",
  "anya-e1007n1:52": "Samaggāca parakkamā;",
  "anya-e1007n1:53": "Mā hotha pupphacumbīva,",
  "anya-e1007n1:54": "Hotha vo pacikā yathā.",
  "anya-e1007n1:55": "1.",
  "anya-e1007n1:56": " Ye maranti kīḷantā te,",
  "anya-e1007n1:57": "Svānā aññoññamoditā;",
  "anya-e1007n1:58": "Disvāna chaṭṭitaṃ bhattaṃ,",
  "anya-e1007n1:59": "Sīghaverī vihiṃsare.",
  "anya-e1007n1:60": "2.",
  "anya-e1007n1:61": " Tathekepi janā dāni,",
  "anya-e1007n1:62": "Samaggā ññoññamoditā;",
  "anya-e1007n1:63": "Dhanahetu vihiṃsanti,",
  "anya-e1007n1:64": "Dhīratthu sīghaverikā.",
  "anya-e1007n1:65": "9.",
  "anya-e1007n1:66": " Vasante hemante gimhe;",
  "anya-e1007n1:67": "Neva tālā visesino;",
  "anya-e1007n1:68": "Thiracittā janā santā;",
  "anya-e1007n1:69": "Sukhadukkhesu niccalā.",
  "anya-e1007n1:70": "10.",
  "anya-e1007n1:71": " Yathā pavaṭṭamānamhi,",
  "anya-e1007n1:72": "Suṭṭhu tiṭṭhati geṇṭhuke;",
  "anya-e1007n1:73": "Appavaṭṭe bhūmyaṃ seti,",
  "anya-e1007n1:74": "Tatheva geṇṭhuko jano.",
  "anya-e1007n1:75": "11.",
  "anya-e1007n1:76": " Agghāpetuṃ nasakkonti,",
  "anya-e1007n1:77": "Kālañhi kālikā janā;",
  "anya-e1007n1:78": "Vajirādiñca sakkonti,",
  "anya-e1007n1:79": "Tena kālo anagghiko.",
  "anya-e1007n1:80": "12. [Ka]",
  "anya-e1007n1:81": " Asano hi dīghaddhāno,",
  "anya-e1007n1:82": "Sārasāro sugandhiko;",
  "anya-e1007n1:83": "Niggandho tveva nissāro,",
  "anya-e1007n1:84": "Dīghaddhānopi simbalī.",
  "anya-e1007n1:85": "[Kha]",
  "anya-e1007n1:86": "Tatheveke janā loke,",
  "anya-e1007n1:87": "Dīghaddhānā susārakā;",
  "anya-e1007n1:88": "Nissārā keci pheggūva,",
  "anya-e1007n1:89": "Dīghaddhānāpi goyathā.",
  "anya-e1007n1:90": "13.",
  "anya-e1007n1:91": " Upakāro cāpakāro,",
  "anya-e1007n1:92": "Yasmiṃ gacchati naṭṭhataṃ;",
  "anya-e1007n1:93": "Pāsāṇahadayassassa,",
  "anya-e1007n1:94": "Jīvatītyā bhidhāmudhā.",
  "anya-e1007n1:95": "Pasādo nipphalo yassa,",
  "anya-e1007n1:96": "Kopocāpi niratthako;",
  "anya-e1007n1:97": "Na taṃ saṅgantu miccheyya,",
  "anya-e1007n1:98": "Thīpumāva napuṃsakaṃ.",
  "anya-e1007n1:99": "14.",
  "anya-e1007n1:100": " Upacāro hi kātabbo,",
  "anya-e1007n1:101": "Na yāva sohadaṃ bhave;",
  "anya-e1007n1:102": "Upacāro sumittamhi,",
  "anya-e1007n1:103": "Māyā ca hoti koṭilaṃ.",
  "anya-e1007n1:104": "15.",
  "anya-e1007n1:105": " Kamena aggato ucchu,",
  "anya-e1007n1:106": "Raso sādutaro yathā;",
  "anya-e1007n1:107": "Tatheva sumitto loke,",
  "anya-e1007n1:108": "Dummitto pana nediso.",
  "anya-e1007n1:109": "16.",
  "anya-e1007n1:110": " Sokārāti parittāṇaṃ,",
  "anya-e1007n1:111": "Vissāsapītibhājanaṃ;",
  "anya-e1007n1:112": "Ratanābhiratanaṃ icche,",
  "anya-e1007n1:113": "Sumittaṃ akkharattayaṃ.",
  "anya-e1007n1:114": "17.",
  "anya-e1007n1:115": " Dampatīnaṃ sumittānaṃ,",
  "anya-e1007n1:116": "Mukhaṃ aññoññadappaṇaṃ,",
  "anya-e1007n1:117": "Sukhe sukhaṃ dukkhe dukkhaṃ,",
  "anya-e1007n1:118": "Paṭicchāyeva dappaṇe.",
  "anya-e1007n1:119": "Kathaṃ nu tāsaṃ hadayaṃ,",
  "anya-e1007n1:120": "Sukharāvata itthiyo;",
  "anya-e1007n1:121": "Yāsāmike dukkhitamhi,",
  "anya-e1007n1:122": "Sukhamicchanti attano.",
  "anya-e1007n1:123": "18.",
  "anya-e1007n1:124": " Nivātañca pure katvā,",
  "anya-e1007n1:125": "Mānaṃ katvāna pacchato;",
  "anya-e1007n1:126": "Sakatthaṃ dhāraye dhīro,",
  "anya-e1007n1:127": "Atthabhañjo hi muḷhatā.",
  "anya-e1007n1:128": "19.",
  "anya-e1007n1:129": " Purecāraṃ satiṃ katvā,",
  "anya-e1007n1:130": "Saddhaṃ kareyya pacchato;",
  "anya-e1007n1:131": "Turaṃ na saddahe dhīro,",
  "anya-e1007n1:132": "Sīghasaddho hi mandako.",
  "anya-e1007n1:133": "20.",
  "anya-e1007n1:134": " Vane 0.0099 bahūni kaṭṭhāni;",
  "anya-e1007n1:135": "Dullabhaṃ rattacandanaṃ;",
  "anya-e1007n1:136": "Tathā janā bahū loke;",
  "anya-e1007n1:137": "Pumā jañño sudullabho.",
  "anya-e1007n1:138": "21.",
  "anya-e1007n1:139": " Tiṇakaṭṭhapalāsehi ;",
  "anya-e1007n1:140": "Sukkhehi dayhate vanaṃ;",
  "anya-e1007n1:141": "Etādīhi asārehi;",
  "anya-e1007n1:142": "Loko janehi dayhate.",
  "anya-e1007n1:143": "22.",
  "anya-e1007n1:144": " Antovasse timāsamhi,",
  "anya-e1007n1:145": "Puññakammena moditā;",
  "anya-e1007n1:146": "Sukhaṃ vasiṃsu porāṇā,",
  "anya-e1007n1:147": "Buddhasāsanamāmakā.",
  "anya-e1007n1:148": "23.",
  "anya-e1007n1:149": " Migamadena ekena,",
  "anya-e1007n1:150": "Taṃ vanaṃ surabhigandhikaṃ;",
  "anya-e1007n1:151": "Tathā janena taṃ raṭṭhaṃ,",
  "anya-e1007n1:152": "Guṇinā hi sirīmatā.",
  "anya-e1007n1:153": "Sarīraṃ khaṇaviddhaṃsī,",
  "anya-e1007n1:154": "Kappantaṭṭhāyino guṇā.",
  "anya-e1007n1:155": "24.",
  "anya-e1007n1:156": " Kharānaṃ sīhabyagghānaṃ,",
  "anya-e1007n1:157": "Saṅgamo no hisabbadā;",
  "anya-e1007n1:158": "Tatheva byagghacittānaṃ,",
  "anya-e1007n1:159": "Sajātikā khayonatā.",
  "anya-e1007n1:160": "25.",
  "anya-e1007n1:161": " Saka sādhupi no sādhū,",
  "anya-e1007n1:162": "Yo ceñña duṭṭhakārako;",
  "anya-e1007n1:163": "Bahūnaṃ sādhū pāyena,",
  "anya-e1007n1:164": "Sa ve sādhūti vuccate.",
  "anya-e1007n1:165": "26.",
  "anya-e1007n1:166": " Bahūdake samuddepi,",
  "anya-e1007n1:167": "Jalaṃ nattheva pātave;",
  "anya-e1007n1:168": "Khuddake khatakūpamhi,",
  "anya-e1007n1:169": "Sāduṃ atthi bahuṃ dakaṃ.",
  "anya-e1007n1:170": "27.",
  "anya-e1007n1:171": " Mā sīghaṃ vivareyyātha,",
  "anya-e1007n1:172": "Nindituñca pasaṃsituṃ;",
  "anya-e1007n1:173": "Mukhañhi vo kathādvāraṃ,",
  "anya-e1007n1:174": "Nirundheyyātha sabbadā.",
  "anya-e1007n1:175": "28.",
  "anya-e1007n1:176": " Mā sīghaṃ vivareyyātha,",
  "anya-e1007n1:177": "Cakkhuṃ vo dassituṃ piyaṃ;",
  "anya-e1007n1:178": "Saṇikañhi piyalābhaṃ,",
  "anya-e1007n1:179": "Dhanalābhaṃ turaṃ kare.",
  "anya-e1007n1:180": "29.",
  "anya-e1007n1:181": " Anārambho hi kammānaṃ,",
  "anya-e1007n1:182": "Paṭhamaṃ buddhilakkhaṇaṃ;",
  "anya-e1007n1:183": "Niṭṭhaṅgataṃ āraddhassa,",
  "anya-e1007n1:184": "Dutiyaṃ buddhilakkhaṇaṃ.",
  "anya-e1007n1:185": "Asamekkhitakammantaṃ ,",
  "anya-e1007n1:186": "Turitābhinipātinaṃ;",
  "anya-e1007n1:187": "Sāni kammāni tappenti,",
  "anya-e1007n1:188": "Uṇhaṃ vajjhohaṭaṃ mukhe.",
  "anya-e1007n1:189": "30.",
  "anya-e1007n1:190": " Aphalāni durantāni,",
  "anya-e1007n1:191": "Janatā ninditāni ca;",
  "anya-e1007n1:192": "Asakyāni ca kammāni,",
  "anya-e1007n1:193": "Nārabhetha vicakkhaṇo.",
  "anya-e1007n1:194": "31.",
  "anya-e1007n1:195": " Ativirodhabhītānaṃ ,",
  "anya-e1007n1:196": "Saṅkitānaṃ pade pade;",
  "anya-e1007n1:197": "Parappavādatāsānaṃ,",
  "anya-e1007n1:198": "Dūrato yanti sampadā.",
  "anya-e1007n1:199": "Saddamattaṃ na bhetabbaṃ,",
  "anya-e1007n1:200": "Loko saddassa gocaro;",
  "anya-e1007n1:201": "Yo ca saddaparittāso,",
  "anya-e1007n1:202": "Vane bhantamigo hi so.",
  "anya-e1007n1:203": "32.",
  "anya-e1007n1:204": " Dvinnaṃ taṇḍulathūsānaṃ,",
  "anya-e1007n1:205": "Viseso suṭṭhu khāyati;",
  "anya-e1007n1:206": "Randhitopi siniddho no,",
  "anya-e1007n1:207": "Thuso virasaphāruso;",
  "anya-e1007n1:208": "Taṇḍulaṃ siniddhaṃ rasaṃ,",
  "anya-e1007n1:209": "Evaṃ lokepi ñāyate.",
  "anya-e1007n1:210": "33.",
  "anya-e1007n1:211": " Eraṇḍaṃ nissitā valli,",
  "anya-e1007n1:212": "Ruhate kiṃ yathābalaṃ;",
  "anya-e1007n1:213": "Mahāsālaṃ sunissāya,",
  "anya-e1007n1:214": "Ruhate brahataṃ gatā.",
  "anya-e1007n1:215": "34.",
  "anya-e1007n1:216": " Mettā hi sīmasambhedā,",
  "anya-e1007n1:217": "Pakkhapāta vighātikā;",
  "anya-e1007n1:218": "Pakkhapātena dukkhanti,",
  "anya-e1007n1:219": "Nippakkho vasate sukhaṃ.",
  "anya-e1007n1:220": "35.",
  "anya-e1007n1:221": " Narā paññā ca laṅkārā,",
  "anya-e1007n1:222": "Yathāṭhāne niyujjare;",
  "anya-e1007n1:223": "No hi cūḷāmaṇi pāde,",
  "anya-e1007n1:224": "Pādukā ca siropari.",
  "anya-e1007n1:225": "Ukkuṭṭhe sūra micchanti;",
  "anya-e1007n1:226": "Mantīsu akutūhalaṃ;",
  "anya-e1007n1:227": "Viyañca annapānamhi;",
  "anya-e1007n1:228": "Atthe jāte ca paṇḍitaṃ.",
  "anya-e1007n1:229": "36.",
  "anya-e1007n1:230": " Pamādo hi tamo loke,",
  "anya-e1007n1:231": "Kālo coro bhayānako;",
  "anya-e1007n1:232": "Kāyagehaṃ bahuchiddaṃ,",
  "anya-e1007n1:233": "Kālacorassa coritaṃ.",
  "anya-e1007n1:234": "37.",
  "anya-e1007n1:235": " Cañcalo kāladāso hi,",
  "anya-e1007n1:236": "Dhītimā kālaissaro;",
  "anya-e1007n1:237": "Kālissaro raṭṭhissaraṃ,",
  "anya-e1007n1:238": "Ativattati sabbaso.",
  "anya-e1007n1:239": "38.",
  "anya-e1007n1:240": " Viluppanti 8 dhanaṃ eke,",
  "anya-e1007n1:241": "Kālameke anekkhakā;",
  "anya-e1007n1:242": "Tesu kālavilopāva,",
  "anya-e1007n1:243": "Bhayānakā tikakkhaḷā.",
  "anya-e1007n1:244": "39.",
  "anya-e1007n1:245": " Raññā raṭṭhahitaṃ kattā,",
  "anya-e1007n1:246": "Rañño hitaṃ janehi ve;",
  "anya-e1007n1:247": "Desso attahitaṃ dassī,",
  "anya-e1007n1:248": "Gārayho kinnu kārako.",
  "anya-e1007n1:249": "Attadatthaṃ paratthena;",
  "anya-e1007n1:250": "Bahunāpi na hāpaye.",
  "anya-e1007n1:251": "40.",
  "anya-e1007n1:252": " Yassa upakāro dinno,",
  "anya-e1007n1:253": "Upakāraṃ dade puna;",
  "anya-e1007n1:254": "Tato pakāraṃ niccheyya,",
  "anya-e1007n1:255": "Kataññū dullabho idha.",
  "anya-e1007n1:256": "Saccaṃ kireva māhaṃsu,",
  "anya-e1007n1:257": "Narā ekacciyā idha;",
  "anya-e1007n1:258": "Kaṭṭhaṃ niplavitaṃ seyyo,",
  "anya-e1007n1:259": "Natveve kacciyo naro.",
  "anya-e1007n1:260": "41.",
  "anya-e1007n1:261": " Kāruko sakapaññāya,",
  "anya-e1007n1:262": "Mahagghaṃ dārukaṃ kare;",
  "anya-e1007n1:263": "Tathā janopi attānaṃ,",
  "anya-e1007n1:264": "Mahaggho lokamānito.",
  "anya-e1007n1:265": "42.",
  "anya-e1007n1:266": " Appaggho hi ayo hemaṃ,",
  "anya-e1007n1:267": "Mahagghaṃ chindate yathā;",
  "anya-e1007n1:268": "Nigguṇo saguṇaṃ loke,",
  "anya-e1007n1:269": "Alakkhīca siriṃ tathā.",
  "anya-e1007n1:270": "Sūrassatīnīti",
  "anya-e1007n1:271": "Dutiyo bhāgo",
  "anya-e1007n1:272": "1.",
  "anya-e1007n1:273": " Dhanassa dubbidhaṃ kiccaṃ,",
  "anya-e1007n1:274": "Pāpeti uṇṇataṃ dhaniṃ;",
  "anya-e1007n1:275": "Adhaniṃ oṇataṃ loke,",
  "anya-e1007n1:276": "Sañcine tena taṃ dhanaṃ.",
  "anya-e1007n1:277": "2.",
  "anya-e1007n1:278": " Vaṭṭate satataṃ sīghaṃ,",
  "anya-e1007n1:279": "Kālacakkaṃ avāritaṃ;",
  "anya-e1007n1:280": "Tena ghaṭī dinaṃ māso,",
  "anya-e1007n1:281": "Vasso bhavatya cīrato.",
  "anya-e1007n1:282": "3.",
  "anya-e1007n1:283": " Sattunā na hi sandheyya,",
  "anya-e1007n1:284": "Ekadā so bhayaṃ karo;",
  "anya-e1007n1:285": "Sutattamapi pānīyaṃ,",
  "anya-e1007n1:286": "Samayate nu pāvakaṃ.",
  "anya-e1007n1:287": "4.",
  "anya-e1007n1:288": " Vijahaṃ pakatiṃ yo hi,",
  "anya-e1007n1:289": "Vikatiṃ puna gacchati;",
  "anya-e1007n1:290": "Sabhāvena ākārena,",
  "anya-e1007n1:291": "Vippallāsaṃ sa gacchati;",
  "anya-e1007n1:292": "Saṃsumāra gatā godhā,",
  "anya-e1007n1:293": "Yathā thī pumavesikā.",
  "anya-e1007n1:294": "5.",
  "anya-e1007n1:295": " Pakkhaṃ laddhāna uḍḍenti,",
  "anya-e1007n1:296": "Upacikā hi vammikā;",
  "anya-e1007n1:297": "Nikkhantā maraṇaṃ yanti,",
  "anya-e1007n1:298": "Uppatā nippataṃ gatā.",
  "anya-e1007n1:299": "6.",
  "anya-e1007n1:300": " Sabbaṃpiyassa dajjeyya,",
  "anya-e1007n1:301": "Nissesaṃ piyamānasaṃ;",
  "anya-e1007n1:302": "Saddhācittaṃ tu no viññū,",
  "anya-e1007n1:303": "Saddhāyiko pakkhalito.",
  "anya-e1007n1:304": "7.",
  "anya-e1007n1:305": " Sakkoti laṅghituṃ byāmaṃ,",
  "anya-e1007n1:306": "Mahussāhena yo hi so;",
  "anya-e1007n1:307": "Tadaḍḍhaṃ anussāhena,",
  "anya-e1007n1:308": "Nossāho tesu thomito;",
  "anya-e1007n1:309": "Mahussāho dukkho loke,",
  "anya-e1007n1:310": "Anussāho sadā sukho.",
  "anya-e1007n1:311": "8.",
  "anya-e1007n1:312": " Dukkhamaṃ akkhamanto yo,",
  "anya-e1007n1:313": "Piṭṭhikārīca dukkaraṃ,",
  "anya-e1007n1:314": "Kadā labheyya so loke,",
  "anya-e1007n1:315": "Sukhamaṃ sukaraṃ mudhā;",
  "anya-e1007n1:316": "Paccakkhañhi so kareyya,",
  "anya-e1007n1:317": "Dukkhamañcāpi dukkaraṃ.",
  "anya-e1007n1:318": "9.",
  "anya-e1007n1:319": " Anaggho manusso loke,",
  "anya-e1007n1:320": "Tosanāposanādinā;",
  "anya-e1007n1:321": "Tena so mahagghaṃ kammaṃ,",
  "anya-e1007n1:322": "Kare lokahitāyutaṃ.",
  "anya-e1007n1:323": "10.",
  "anya-e1007n1:324": " Vātena nappabhijjanti,",
  "anya-e1007n1:325": "Ninnā veḷū kasā naḷā;",
  "anya-e1007n1:326": "Yathāvātaṃ nagacchanti,",
  "anya-e1007n1:327": "Tathā care jane kadā.",
  "anya-e1007n1:328": "11.",
  "anya-e1007n1:329": " Purato ca pacchato ce,",
  "anya-e1007n1:330": "Nissayo natthi passato;",
  "anya-e1007n1:331": "Adhikaṃ vīriyaṃ hoti,",
  "anya-e1007n1:332": "Attanātho tadā bhave.",
  "anya-e1007n1:333": "12.",
  "anya-e1007n1:334": " Khaṇaṃ ākhubilaṃ sīho,",
  "anya-e1007n1:335": "Pāsāṇasakalākulaṃ;",
  "anya-e1007n1:336": "Pappoti nakhabhaṅgaṃvā,",
  "anya-e1007n1:337": "Phalaṃvā mūsiko bhave.",
  "anya-e1007n1:338": "13.",
  "anya-e1007n1:339": " Maggamuḷhā janā andhā,",
  "anya-e1007n1:340": "Amaggā maggasaññino;",
  "anya-e1007n1:341": "Tatheva duppaññā muḷhā,",
  "anya-e1007n1:342": "Tathatthaṃ nāva bujjhare.",
  "anya-e1007n1:343": "14.",
  "anya-e1007n1:344": " Piyarūpaṃ vīrarūpaṃ,",
  "anya-e1007n1:345": "Duvidhā rūpasampadā,",
  "anya-e1007n1:346": "Nāriṃ icche piyarūpiṃ,",
  "anya-e1007n1:347": "Purisaṃ vīrarūpakaṃ.",
  "anya-e1007n1:348": "15.",
  "anya-e1007n1:349": " Pajjalanti hi khajjotā,",
  "anya-e1007n1:350": "Pakkhacālanakammunā;",
  "anya-e1007n1:351": "Kusitā supitā nete,",
  "anya-e1007n1:352": "Tathā janāpi kammikā.",
  "anya-e1007n1:353": "16.",
  "anya-e1007n1:354": " Kummasaṅkocamopamya ,",
  "anya-e1007n1:355": "Niggahamapi saṃkhame;",
  "anya-e1007n1:356": "Pattakāle tu nītiñño,",
  "anya-e1007n1:357": "Kaṇhasappova uṭṭhahe.",
  "anya-e1007n1:358": "17.",
  "anya-e1007n1:359": " Bhakkhasesaṃ nakhādanti,",
  "anya-e1007n1:360": "Sīhā unnatacetasā;",
  "anya-e1007n1:361": "Paraṃpi napaṇāmenti,",
  "anya-e1007n1:362": "Vuddhikāmā tathā care.",
  "anya-e1007n1:363": "18.",
  "anya-e1007n1:364": " Na va vasso samuppanno,",
  "anya-e1007n1:365": "Khīṇo purāṇahāyano;",
  "anya-e1007n1:366": "Navavasse navā mettā,",
  "anya-e1007n1:367": "Bhāvitabbā hitesinā.",
  "anya-e1007n1:368": "19.",
  "anya-e1007n1:369": " Santāpayanti kamayāpyabhujaṃ na rogā,",
  "anya-e1007n1:370": "Dummantinaṃ kamupayanti na nītidosā;",
  "anya-e1007n1:371": "Kaṃ srī na mānayati kaṃ na ca hanti maccu,",
  "anya-e1007n1:372": "Kaṃ thīkatā na visayā paripīḷayanti.",
  "anya-e1007n1:373": "(Vasantatilakāgāthā.)",
  "anya-e1007n1:374": "20.",
  "anya-e1007n1:375": " (Ka) byāmamattena daṇḍena,",
  "anya-e1007n1:376": "Yolumbya udakaṃ mine;",
  "anya-e1007n1:377": "Agambhīraṃ gambhīraṃvā,",
  "anya-e1007n1:378": "Agādhe maññi gambhīraṃ.",
  "anya-e1007n1:379": "(Kha)",
  "anya-e1007n1:380": "Tathā mando saññāṇena,",
  "anya-e1007n1:381": "Agādhe maññi paṇḍitaṃ;",
  "anya-e1007n1:382": "Samāsamaṃ na jānāti,",
  "anya-e1007n1:383": "Bahvappaṃ tikkhamandataṃ.",
  "anya-e1007n1:384": "21.",
  "anya-e1007n1:385": " Duṭṭhakamme saṅgamanti,",
  "anya-e1007n1:386": "Chekakamme ca no idha;",
  "anya-e1007n1:387": "Maccuṃ vahanti sīsena,",
  "anya-e1007n1:388": "Te muḷhā muḷhasaṅgamā.",
  "anya-e1007n1:389": "22.",
  "anya-e1007n1:390": " Duvidho saṅgamo loke,",
  "anya-e1007n1:391": "Ujuko kuṭilo bhave;",
  "anya-e1007n1:392": "Ujukova pasaṃseyyo,",
  "anya-e1007n1:393": "Nohyañño sājasaṅgamo.",
  "anya-e1007n1:394": "Te iminā upāyena samaggā sammodamānā mahā bhittipiṭṭhikāya vasanti. (Mahosadhajātaka aṭṭhakathā)",
  "anya-e1007n1:395": "23.",
  "anya-e1007n1:396": " Yūthikā pupphate nohi,",
  "anya-e1007n1:397": "Siñcitāpi punappunaṃ;",
  "anya-e1007n1:398": "Pupphate sampatte kāle,",
  "anya-e1007n1:399": "Evaṃ dhāretha vīriyaṃ.",
  "anya-e1007n1:400": "24.",
  "anya-e1007n1:401": " Dhanuccayo dhanakkhepo,",
  "anya-e1007n1:402": "Duvidhā hi dhanākati;",
  "anya-e1007n1:403": "Dhanuccaye nayo atthi,",
  "anya-e1007n1:404": "Dhanakkhepamhi no idha.",
  "anya-e1007n1:405": "25.",
  "anya-e1007n1:406": " Amātā pitarasaṃ vaḍḍhaṃ,",
  "anya-e1007n1:407": "Jūtakārañca cañcalaṃ;",
  "anya-e1007n1:408": "Nālapeyya visesaññū,",
  "anya-e1007n1:409": "Yadicche siddhi mattano.",
  "anya-e1007n1:410": "Haliddirāgaṃ kapicittaṃ,",
  "anya-e1007n1:411": "Purisaṃ rāgavirāginaṃ;",
  "anya-e1007n1:412": "Edisaṃ tāta māsevi,",
  "anya-e1007n1:413": "Nimmanussaṃpi ce siyā.",
  "anya-e1007n1:414": "26.",
  "anya-e1007n1:415": " Guṇā guṇaññūsu guṇā bhavanti,",
  "anya-e1007n1:416": "Te nigguṇaṃ patvā bhavanti dosā;",
  "anya-e1007n1:417": "Āsādyatoyā pabhavanti najjo,",
  "anya-e1007n1:418": "Samudramāsajja bhavantyapeyyā.",
  "anya-e1007n1:419": "(Upajātigāthā)",
  "anya-e1007n1:420": "27.",
  "anya-e1007n1:421": " Silārūpaṃ nimminanti,",
  "anya-e1007n1:422": "Koṭṭetvāna punappunaṃ;",
  "anya-e1007n1:423": "Koṭṭakova tathā bālā,",
  "anya-e1007n1:424": "Sādhuṃ ovajja nimmitā."
}