{
  "anya-e1009n12:0": "Sīla-niddesa",
  "anya-e1009n12:1": "291.",
  "anya-e1009n12:2": " Pamādaṃ bhayato disvā,",
  "anya-e1009n12:3": "A-ppamādañca khemato;",
  "anya-e1009n12:4": "Bhāvetha aṭṭhaṅgikaṃ maggaṃ,",
  "anya-e1009n12:5": "Esā buddhānusāsanī.",
  "anya-e1009n12:6": "292.",
  "anya-e1009n12:7": " Hīnena brahma-cariyena,",
  "anya-e1009n12:8": "Khattiye upapajjati;",
  "anya-e1009n12:9": "Majjhimena ca devattaṃ,",
  "anya-e1009n12:10": "Uttamena vi-sujjhati.",
  "anya-e1009n12:11": "Ka.",
  "anya-e1009n12:12": " Nagare bandhumatiyā,",
  "anya-e1009n12:13": "Bandhumā nāma khattiyo;",
  "anya-e1009n12:14": "Divase puṇṇamāya so,",
  "anya-e1009n12:15": "Upagacchi uposathaṃ.",
  "anya-e1009n12:16": "Kha.",
  "anya-e1009n12:17": " Ahaṃ tena samayena,",
  "anya-e1009n12:18": "Gumbha-dāsī ahaṃ tahiṃ;",
  "anya-e1009n12:19": "Disvā sa-rājakaṃ senaṃ,",
  "anya-e1009n12:20": "Evāhaṃ cintayiṃ tadā.",
  "anya-e1009n12:21": "Ga.",
  "anya-e1009n12:22": " Rājāpi rajjaṃ chaṭṭetvā,",
  "anya-e1009n12:23": "Upagacchi uposathaṃ;",
  "anya-e1009n12:24": "Sa-phalaṃ nūna taṃ kammaṃ,",
  "anya-e1009n12:25": "Jana-kāyo pamodito.",
  "anya-e1009n12:26": "Gha.",
  "anya-e1009n12:27": " Yoniso paccavekkhitvā,",
  "anya-e1009n12:28": "Duggaccañca daliddataṃ;",
  "anya-e1009n12:29": "Mānasaṃ sampahaṃsitvā,",
  "anya-e1009n12:30": "Upagacchimu posathaṃ.",
  "anya-e1009n12:31": "Ṅa.",
  "anya-e1009n12:32": " Ahaṃ uposathaṃ katvā,",
  "anya-e1009n12:33": "Sammā-sambuddhasāsane;",
  "anya-e1009n12:34": "Tena kammena su-katena,",
  "anya-e1009n12:35": "Tāvatiṃsaṃ agacchahaṃ.",
  "anya-e1009n12:36": "Ca.",
  "anya-e1009n12:37": " Tattha me su-kataṃ byamhaṃ,",
  "anya-e1009n12:38": "Ubbha-yojanamuggataṃ;",
  "anya-e1009n12:39": "Kūṭāgāra-varūpetaṃ,",
  "anya-e1009n12:40": "Mahāsanasu-bhūsitaṃ.",
  "anya-e1009n12:41": "Cha.",
  "anya-e1009n12:42": " Accharā sata-sahassā,",
  "anya-e1009n12:43": "Upatiṭṭhanti maṃ sadā;",
  "anya-e1009n12:44": "Aññe deve atikkamma,",
  "anya-e1009n12:45": "Atirocāmi sabbadā.",
  "anya-e1009n12:46": "Ja.",
  "anya-e1009n12:47": " Catusaṭṭhi-deva-rājūnaṃ ,",
  "anya-e1009n12:48": "Mahesittamakārayiṃ;",
  "anya-e1009n12:49": "Tesaṭṭhi-cakkavattinaṃ,",
  "anya-e1009n12:50": "Mahesittamakārayiṃ.",
  "anya-e1009n12:51": "Jha.",
  "anya-e1009n12:52": " Suvaṇṇa-vaṇā hutvāna,",
  "anya-e1009n12:53": "Bhavesu saṃsarāmahaṃ;",
  "anya-e1009n12:54": "Sabbattha pavarā homi,",
  "anya-e1009n12:55": "Uposathassidaṃ phalaṃ.",
  "anya-e1009n12:56": "Ña.",
  "anya-e1009n12:57": " Hatthi-yānaṃ assa-yānaṃ,",
  "anya-e1009n12:58": "Ratha-yānañca sivikaṃ;",
  "anya-e1009n12:59": "Labhāmi sabbametampi,",
  "anya-e1009n12:60": "Uposathassidaṃ phalaṃ.",
  "anya-e1009n12:61": "Ṭa.",
  "anya-e1009n12:62": " Soṇṇa-mayaṃ rūpi-mayaṃ,",
  "anya-e1009n12:63": "Athopi phalikā-mayaṃ;",
  "anya-e1009n12:64": "Lohitaṅga-mayañceva,",
  "anya-e1009n12:65": "Sabbaṃ paṭilabhāmahaṃ.",
  "anya-e1009n12:66": "Ṭha.",
  "anya-e1009n12:67": " Koseyya-kambaliyāni ,",
  "anya-e1009n12:68": "Khoma-kappāsikāni ca;",
  "anya-e1009n12:69": "Mahagghāni ca vatthāni,",
  "anya-e1009n12:70": "Sabbaṃ paṭilabhāmahaṃ.",
  "anya-e1009n12:71": "Ḍa.",
  "anya-e1009n12:72": " Annaṃ pānaṃ khādanīyaṃ,",
  "anya-e1009n12:73": "Vatthaṃ senāsanāni ca;",
  "anya-e1009n12:74": "Sabbametaṃ paṭilabhe,",
  "anya-e1009n12:75": "Uposathassidaṃ phalaṃ.",
  "anya-e1009n12:76": "Ḍha.",
  "anya-e1009n12:77": " Vara-gandhañca mālañca,",
  "anya-e1009n12:78": "Cuṇṇakañcha vilepanaṃ;",
  "anya-e1009n12:79": "Sabbametaṃ paṭilabhe,",
  "anya-e1009n12:80": "Uposathassidaṃ phalaṃ.",
  "anya-e1009n12:81": "Ṇa.",
  "anya-e1009n12:82": " Kūṭāgārañca pāsādaṃ,",
  "anya-e1009n12:83": "Maṇḍapaṃ hammiyaṃ guhaṃ;",
  "anya-e1009n12:84": "Sabbametaṃ paṭilabhe,",
  "anya-e1009n12:85": "Uposathassidaṃ phalaṃ.",
  "anya-e1009n12:86": "Ta.",
  "anya-e1009n12:87": " Jātiyā satta-vassāhaṃ,",
  "anya-e1009n12:88": "Pabbajiṃ ana-gāriyaṃ;",
  "anya-e1009n12:89": "Aḍḍha-māse a-sappatte,",
  "anya-e1009n12:90": "Ara hattaṃ apāpuṇiṃ.",
  "anya-e1009n12:91": "Tha.",
  "anya-e1009n12:92": " Kilesā jhāpitā mayhaṃ,",
  "anya-e1009n12:93": "Bhavā sabbe samūhatā;",
  "anya-e1009n12:94": "Sabbāsava-parikkhīṇā,",
  "anya-e1009n12:95": "Natthi dāni puna-bbhavo.",
  "anya-e1009n12:96": "Da.",
  "anya-e1009n12:97": " Eka-navutito kappe,",
  "anya-e1009n12:98": "Yaṃ kammamakariṃ tadā;",
  "anya-e1009n12:99": "Duggatiṃ nābhijānāmi,",
  "anya-e1009n12:100": "Uposathassidaṃ phalaṃ.",
  "anya-e1009n12:101": "Dha.",
  "anya-e1009n12:102": " Svāgataṃ vata me āsi,",
  "anya-e1009n12:103": "Mama buddhassa santike;",
  "anya-e1009n12:104": "Tisso vijjā anu-pattā,",
  "anya-e1009n12:105": "Kataṃ buddhassa sāsanaṃ.",
  "anya-e1009n12:106": "Na.",
  "anya-e1009n12:107": " Paṭisambhidā catasso,",
  "anya-e1009n12:108": "Vimokāpi ca aṭṭhime;",
  "anya-e1009n12:109": "Chaḷābhiññā sacchikatā,",
  "anya-e1009n12:110": "Kataṃ buddhassa sāsanaṃ.",
  "anya-e1009n12:111": "293.",
  "anya-e1009n12:112": " Ñātīnañca piyo hoti,",
  "anya-e1009n12:113": "Mittesu ca virocati;",
  "anya-e1009n12:114": "Kāyassa bhedā su-gatiṃ,",
  "anya-e1009n12:115": "Upapajjati sīlavā.",
  "anya-e1009n12:116": "294.",
  "anya-e1009n12:117": " Nibbānaṃ patthayantena samādinnaṃ,",
  "anya-e1009n12:118": "Pañca-sīlampi adhi-sīlaṃ;",
  "anya-e1009n12:119": "Dasa-sīlampi adhi-sīlameva.",
  "anya-e1009n12:120": "Ka.",
  "anya-e1009n12:121": " Nagare candavatiyā,",
  "anya-e1009n12:122": "Bhaṭako āsahaṃ tadā;",
  "anya-e1009n12:123": "Para-kammāyane yutto,",
  "anya-e1009n12:124": "Pabbajjaṃ na labhāmahaṃ.",
  "anya-e1009n12:125": "Kha.",
  "anya-e1009n12:126": " Mahandhakāra-pihitā ,",
  "anya-e1009n12:127": "Tividhaggīhi ḍayhare;",
  "anya-e1009n12:128": "Kena nukho upāyena,",
  "anya-e1009n12:129": "Vi-saṃyutto bhave ahaṃ.",
  "anya-e1009n12:130": "Ga.",
  "anya-e1009n12:131": " Deyyadhammo ca me natthi,",
  "anya-e1009n12:132": "Bhaṭako dukkhito ahaṃ;",
  "anya-e1009n12:133": "Yaṃ nūnāhaṃ pañca-sīlaṃ,",
  "anya-e1009n12:134": "Rakkheyyaṃ paripūrayaṃ.",
  "anya-e1009n12:135": "Gha.",
  "anya-e1009n12:136": " Anomadassissa munino,",
  "anya-e1009n12:137": "Nisabho nāma sāvako;",
  "anya-e1009n12:138": "Tamahaṃ upasaṅkamma,",
  "anya-e1009n12:139": "Pañca-sikkhāpadaggahiṃ.",
  "anya-e1009n12:140": "Ṅa.",
  "anya-e1009n12:141": " Vassa-sata-sahassāni ,",
  "anya-e1009n12:142": "Āyu vijjati tāvade;",
  "anya-e1009n12:143": "Tāvatā pañca-sīlāni,",
  "anya-e1009n12:144": "Paripuṇṇāni gopayiṃ.",
  "anya-e1009n12:145": "Ca.",
  "anya-e1009n12:146": " Maccu-kālamhi sampatte,",
  "anya-e1009n12:147": "Devā assāsayanti maṃ;",
  "anya-e1009n12:148": "Ratho sahassa-yutto te,",
  "anya-e1009n12:149": "Mārisassa upaṭṭhito.",
  "anya-e1009n12:150": "Cha.",
  "anya-e1009n12:151": " Vattante carime citte,",
  "anya-e1009n12:152": "Mama sīlaṃ anussariṃ;",
  "anya-e1009n12:153": "Tena kammena su-katena,",
  "anya-e1009n12:154": "Tāvatiṃsaṃ agacchahaṃ.",
  "anya-e1009n12:155": "Ja.",
  "anya-e1009n12:156": " Tiṃsakhattuñca devindo,",
  "anya-e1009n12:157": "Deva-rajjamakārayiṃ;",
  "anya-e1009n12:158": "Dibba-sukhaṃ anubhaviṃ,",
  "anya-e1009n12:159": "Accharāhi purakkhatto.",
  "anya-e1009n12:160": "Jha.",
  "anya-e1009n12:161": " Pañca-sattatikhatttu-ñca ,",
  "anya-e1009n12:162": "Cakkavattī ahosahaṃ;",
  "anya-e1009n12:163": "Padesa-rajjaṃ vipulaṃ,",
  "anya-e1009n12:164": "Gaṇanāto a-saṅkhayaṃ.",
  "anya-e1009n12:165": "Ña.",
  "anya-e1009n12:166": " Deva-lokā cavitvāna,",
  "anya-e1009n12:167": "Sukka-mūlena codito;",
  "anya-e1009n12:168": "Pure vesāliyaṃ jāto,",
  "anya-e1009n12:169": "Mahā-kule su-aḍḍhake.",
  "anya-e1009n12:170": "Ṭa.",
  "anya-e1009n12:171": " Vassūpanāyike kāle,",
  "anya-e1009n12:172": "Dibbante jina-sāsane;",
  "anya-e1009n12:173": "Mātā ca me pitā ceva,",
  "anya-e1009n12:174": "Pañca-sikkhāpadaggahuṃ.",
  "anya-e1009n12:175": "Ṭha.",
  "anya-e1009n12:176": " Saha sutvānahaṃ sīlaṃ,",
  "anya-e1009n12:177": "Mama sīlaṃ anussariṃ;",
  "anya-e1009n12:178": "Ekāsane nisīditvā,",
  "anya-e1009n12:179": "Arahattamapāpuṇiṃ.",
  "anya-e1009n12:180": "Ḍa.",
  "anya-e1009n12:181": " Jātiyā pañca-vassena,",
  "anya-e1009n12:182": "Arahattamapāpuṇiṃ;",
  "anya-e1009n12:183": "Upasampādayi buddho,",
  "anya-e1009n12:184": "Guṇamaññāya cakkhumā.",
  "anya-e1009n12:185": "Ḍha.",
  "anya-e1009n12:186": " Paripuṇṇāni gopetvā,",
  "anya-e1009n12:187": "Pañca-sikkhāpadānahaṃ;",
  "anya-e1009n12:188": "A-parimeyyito kappe,",
  "anya-e1009n12:189": "Vinipātaṃ na gacchahaṃ.",
  "anya-e1009n12:190": "Ṇa.",
  "anya-e1009n12:191": " Svāhaṃ yasamanubhaviṃ,",
  "anya-e1009n12:192": "Tesaṃ sīlāna vāhasā;",
  "anya-e1009n12:193": "Kappa-koṭipi kittento,",
  "anya-e1009n12:194": "Kittaye eka-desakaṃ.",
  "anya-e1009n12:195": "Ta.",
  "anya-e1009n12:196": " Pañca-sīlāni gopetvā,",
  "anya-e1009n12:197": "Tayo hetū labhāmahaṃ;",
  "anya-e1009n12:198": "Dīghāyuko mahā-bhogo,",
  "anya-e1009n12:199": "Tikkha-pañño bhavāmahaṃ.",
  "anya-e1009n12:200": "Tha.",
  "anya-e1009n12:201": " Saṃkittento ca sabbesaṃ,",
  "anya-e1009n12:202": "Adhi-mattañca porisaṃ;",
  "anya-e1009n12:203": "Bhavābhave saṃsaritvā,",
  "anya-e1009n12:204": "Ete ṭhāne labhamahaṃ.",
  "anya-e1009n12:205": "Da.",
  "anya-e1009n12:206": " A-parimeyya-sīlesu ,",
  "anya-e1009n12:207": "Vattantā jina-sāvakā;",
  "anya-e1009n12:208": "Bhavesu yadi rajjeyyuṃ,",
  "anya-e1009n12:209": "Vipāko kīdiso bhave.",
  "anya-e1009n12:210": "Dha.",
  "anya-e1009n12:211": " Su-ciṇṇaṃ me pañca-sīlaṃ,",
  "anya-e1009n12:212": "Bhaṭakena tapassinā;",
  "anya-e1009n12:213": "Tena sīlenahaṃ ajja,",
  "anya-e1009n12:214": "Mocayiṃ sabba-bandhanā.",
  "anya-e1009n12:215": "Na.",
  "anya-e1009n12:216": " A-parimeyyito kappe,",
  "anya-e1009n12:217": "Pañca-sīlāni gopayiṃ;",
  "anya-e1009n12:218": "Duggatiṃ nābhijānāmi,",
  "anya-e1009n12:219": "Pañca-sīlānidaṃ phalaṃ.",
  "anya-e1009n12:220": "Pa.",
  "anya-e1009n12:221": " Paṭisambhidā catasso,",
  "anya-e1009n12:222": "Vimokkhāpi ca aṭṭhime;",
  "anya-e1009n12:223": "Chaḷābhiññā sacchikatā,",
  "anya-e1009n12:224": "Kataṃ buddhassa sāsanaṃ.",
  "anya-e1009n12:225": "Ka.",
  "anya-e1009n12:226": " Taṃ namassanti te vijjā,",
  "anya-e1009n12:227": "Sabbe bhūmā ca khattiyā;",
  "anya-e1009n12:228": "Cattāro ca mahā-rājā,",
  "anya-e1009n12:229": "Tidasā ca yasassino;",
  "anya-e1009n12:230": "Atha ko nāma so yakkho,",
  "anya-e1009n12:231": "Yaṃ tvaṃ sakka namassasi.",
  "anya-e1009n12:232": "Kha.",
  "anya-e1009n12:233": " Maṃ namassanti te-vijjā,",
  "anya-e1009n12:234": "Sabbe bhūmā ca khattiyā;",
  "anya-e1009n12:235": "Cattāro ca mahā-rājā,",
  "anya-e1009n12:236": "Tidasā ca yasassino.",
  "anya-e1009n12:237": "Ga.",
  "anya-e1009n12:238": " Ahañca sīla-sampanne,",
  "anya-e1009n12:239": "Ciraratta-samāhite;",
  "anya-e1009n12:240": "Sammā pabbajite vande,",
  "anya-e1009n12:241": "Brahma-cariya parāyane.",
  "anya-e1009n12:242": "Gha.",
  "anya-e1009n12:243": " Ye gahaṭṭhā puñña-karā,",
  "anya-e1009n12:244": "Sīlavanto upāsakā;",
  "anya-e1009n12:245": "Dhammena dāraṃ posenti,",
  "anya-e1009n12:246": "Te namassāmi mātali.",
  "anya-e1009n12:247": "Ṅa.",
  "anya-e1009n12:248": " Seṭṭhā hi kira lokasmiṃ,",
  "anya-e1009n12:249": "Ye tvaṃ sakka namassasi;",
  "anya-e1009n12:250": "Ahampi te namassāmi,",
  "anya-e1009n12:251": "Ye namassasi vāsava.",
  "anya-e1009n12:252": "1.",
  "anya-e1009n12:253": " Pālitattheranāgena ,",
  "anya-e1009n12:254": "Visuddhārāmavāsinā;",
  "anya-e1009n12:255": "Suticchitānamatthāya,",
  "anya-e1009n12:256": "Katā naradakkhadīpanī.",
  "anya-e1009n12:257": "2.",
  "anya-e1009n12:258": " Pubbācariya-sīhānaṃ ,",
  "anya-e1009n12:259": "Ālambitvāna nissayaṃ;",
  "anya-e1009n12:260": "Pālito nāma yo thero,",
  "anya-e1009n12:261": "Imaṃ gantha su-lekhanī;",
  "anya-e1009n12:262": "Sundarameva passituṃ,",
  "anya-e1009n12:263": "Yuñjeyyāthīdha sādhave.",
  "anya-e1009n12:264": "‘‘Chappadikā’’.",
  "anya-e1009n12:265": "3.",
  "anya-e1009n12:266": " Imaṃ ganthaṃ vācuggato,",
  "anya-e1009n12:267": "Sace bhavasi māṇava;",
  "anya-e1009n12:268": "Puṇnamāyaṃ yathā cando,",
  "anya-e1009n12:269": "Ati-suddho virocati;",
  "anya-e1009n12:270": "Tatheva tvaṃ puṇṇa-mano,",
  "anya-e1009n12:271": "Viroca siriyā dhuvaṃ.",
  "anya-e1009n12:272": "4.",
  "anya-e1009n12:273": " Su-niṭṭhito ayaṃ gantho,",
  "anya-e1009n12:274": "Sakkarāje dajhamphiye;",
  "anya-e1009n12:275": "Poṭṭhapādamhi sūramhi,",
  "anya-e1009n12:276": "Kālapakkhe catuddasiṃ.",
  "anya-e1009n12:277": "5.",
  "anya-e1009n12:278": " Sañcitetaṃ mayā puññaṃ,",
  "anya-e1009n12:279": "Taṃ-kammena varena ca;",
  "anya-e1009n12:280": "Ciraṃ tiṭṭhatu saddhammo,",
  "anya-e1009n12:281": "A-verā hontu pāṇino.",
  "anya-e1009n12:282": "6.",
  "anya-e1009n12:283": " Imaṃ ganthaṃ passitvāna,",
  "anya-e1009n12:284": "Hontu sabbepi jantuno;",
  "anya-e1009n12:285": "Sukhitā dhammikā ñāṇī,",
  "anya-e1009n12:286": "Dhammaṃ pāletu patthivo.",
  "anya-e1009n12:287": "7.",
  "anya-e1009n12:288": " Nibbānaṃ patthayantena,",
  "anya-e1009n12:289": "Sīlaṃ rakkhantu sajjanā;",
  "anya-e1009n12:290": "Ñatvā dhammaṃ sukhāvahaṃ,",
  "anya-e1009n12:291": "Pāpuṇantu anāsavaṃ.",
  "anya-e1009n12:292": "8.",
  "anya-e1009n12:293": " Aṭṭha-kaṇḍa-maṇḍitāya ,",
  "anya-e1009n12:294": "Dakkhaya attha-dīpako;",
  "anya-e1009n12:295": "Nara-sāro ayaṃ gantho,",
  "anya-e1009n12:296": "Cira-kālaṃ patiṭṭhatu.",
  "anya-e1009n12:297": "9.",
  "anya-e1009n12:298": " Yāvatā canda-sūriyā,",
  "anya-e1009n12:299": "Nāgaccheyyuṃ mahī-tale;",
  "anya-e1009n12:300": "Pamoditā imaṃ ganthaṃ,",
  "anya-e1009n12:301": "Dissantu naya-kovidā.",
  "anya-e1009n12:302": "10.",
  "anya-e1009n12:303": " Sammā chandenimaṃ gantha,",
  "anya-e1009n12:304": "Vācentā pariyāpuṇā;",
  "anya-e1009n12:305": "Pasannenānāyāsena,",
  "anya-e1009n12:306": "Patvā sukhena kovidaṃ.",
  "anya-e1009n12:307": "11.",
  "anya-e1009n12:308": " Candādiccāva ākāse,",
  "anya-e1009n12:309": "Bahussutehi sampadā;",
  "anya-e1009n12:310": "Visesa-puggalā hutvā,",
  "anya-e1009n12:311": "Pappontu amataṃ padaṃ.",
  "anya-e1009n12:312": "12.",
  "anya-e1009n12:313": " Ukkaṭṭha-dhamma-dānena ,",
  "anya-e1009n12:314": "Pāpuṇeyyamanuttaraṃ;",
  "anya-e1009n12:315": "Liṅga-sampatti-medhāvī,",
  "anya-e1009n12:316": "Takkī-paññā su-pesalī."
}