{
  "anya-e1101n0:0": "Namo tassa bhagavato arahato sammāsambuddhassa.",
  "anya-e1101n0:1": "Rasavāhinī",
  "anya-e1101n0:2": "Paṇāmādikathā",
  "anya-e1101n0:3": "1.",
  "anya-e1101n0:4": " Satthuppasatthacaraṇaṃ saraṇaṃ janānaṃ,",
  "anya-e1101n0:5": "Brahmādimoḷi maṇiraṃsi samāvahantaṃ,",
  "anya-e1101n0:6": "Paṅkeruhābhamudukomalacāruvaṇṇaṃ;",
  "anya-e1101n0:7": "Vandāmi cakkavaralakkhaṇamādadhānaṃ.",
  "anya-e1101n0:8": "2.",
  "anya-e1101n0:9": " Siddhaṃ jinena cirakālamatanditena,",
  "anya-e1101n0:10": "Yaṃ bhāvako samadhigacchati khemamaggaṃ;",
  "anya-e1101n0:11": "Yaṃ kapparukkha rucidāna maṇiva bhāti,",
  "anya-e1101n0:12": "Taṃ dhammamagga masamaṃ paṇamāmi niccaṃ.",
  "anya-e1101n0:13": "3.",
  "anya-e1101n0:14": " Santindriyaṃ sugatasūnuvaraṃ visuddhaṃ,",
  "anya-e1101n0:15": "Yaṃ dakkhiṇeyyamatadaṃ sucipuññakhettaṃ;",
  "anya-e1101n0:16": "Tāṇesinaṃ saraṇamujjhitasabbadukkhaṃ,",
  "anya-e1101n0:17": "Vandāmi saṅgha managhaṃ sirasā mahagghaṃ.",
  "anya-e1101n0:18": "4.",
  "anya-e1101n0:19": " Yampattamettha ratanattayathomanena,",
  "anya-e1101n0:20": "Puññena tena duritaṃ sakalaṃ paṇujja,",
  "anya-e1101n0:21": "Vakkhāmahaṃ sumadhuraṃ rasavāhinintaṃ,",
  "anya-e1101n0:22": "Bho bho suṇantu sujanā bhimudāvahā sā.",
  "anya-e1101n0:23": "5.",
  "anya-e1101n0:24": " Tatthatatthūpapannāni, vatthūni arahā pure;",
  "anya-e1101n0:25": "Abhāsuṃ dīpabhāsāya, ṭhapesuṃ taṃ purātanā.",
  "anya-e1101n0:26": "6.",
  "anya-e1101n0:27": " Mahāvihāre taṅgutta, vaṅkapariveṇavāsiko;",
  "anya-e1101n0:28": "Raṭṭhapāloti nāmena, sīlācāra guṇākaro.",
  "anya-e1101n0:29": "7.",
  "anya-e1101n0:30": " Hitāya parivattesi, pajānaṃ pāḷibhāsato;",
  "anya-e1101n0:31": "Punaruttādidosehi, tamāsi sabbamākulaṃ;",
  "anya-e1101n0:32": "Anākulaṃ karissāmi, taṃ suṇātha samāhitā.",
  "anya-e1101n0:33": "8.",
  "anya-e1101n0:34": " Vitarāgā pure vocuṃ, yasmā tasmā hi bhāsitaṃ;",
  "anya-e1101n0:35": "Etamādaraṇīyañhi, sādhu sādhūhi sabbadāti.",
  "anya-e1101n0:36": "Jambudīpuppatti vatthūni."
}