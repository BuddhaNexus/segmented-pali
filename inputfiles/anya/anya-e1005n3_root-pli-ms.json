{
  "anya-e1005n3:0": "4. Mittakaṇḍo",
  "anya-e1005n3:1": "79.",
  "anya-e1005n3:2": " Hitakāro paro bandhu,",
  "anya-e1005n3:3": "Bandhūpi ahito paro;",
  "anya-e1005n3:4": "A hito dehajo byādhi,",
  "anya-e1005n3:5": "Hitaṃ araññamosadhaṃ.",
  "anya-e1005n3:6": "80.",
  "anya-e1005n3:7": " Parokkhe guṇa-hantāraṃ,",
  "anya-e1005n3:8": "Paccakkhe piyavādinaṃ;",
  "anya-e1005n3:9": "Vajjeyya tādisaṃ mittaṃ,",
  "anya-e1005n3:10": "Visakumbhe yathā madhuṃ.",
  "anya-e1005n3:11": "81.",
  "anya-e1005n3:12": " Dhanahīne caje mitto,",
  "anya-e1005n3:13": "Puttadārā sahodarā;",
  "anya-e1005n3:14": "Dhanavantaṃva sevanti,",
  "anya-e1005n3:15": "Dhanaṃ loke mahā sakhā.",
  "anya-e1005n3:16": "82.",
  "anya-e1005n3:17": " Jāneyya pesena bhaccaṃ,",
  "anya-e1005n3:18": "Bandhuṃ vāpi bhayāgate;",
  "anya-e1005n3:19": "Appakāsu tathā mittaṃ,",
  "anya-e1005n3:20": "Dārañca vibhavakkhaye.",
  "anya-e1005n3:21": "83.",
  "anya-e1005n3:22": " So bandhu yo hite yutto,",
  "anya-e1005n3:23": "Pitaro honti posako;",
  "anya-e1005n3:24": "Taṃ mittaṃ yattha vissāso,",
  "anya-e1005n3:25": "Sā bhariyā ca yassa nibbūti.",
  "anya-e1005n3:26": "84.",
  "anya-e1005n3:27": " Na vissase a-vissattaṃ,",
  "anya-e1005n3:28": "Mittañcāpi na vissase;",
  "anya-e1005n3:29": "Kadāci kupito mito,",
  "anya-e1005n3:30": "Sabbaṃ dosaṃ pakāsaye.",
  "anya-e1005n3:31": "85.",
  "anya-e1005n3:32": " Sakiṃ duṭṭhañca yo mittaṃ,",
  "anya-e1005n3:33": "Puna sandhitumicchati;",
  "anya-e1005n3:34": "So maccuṃ upagaṇhāti,",
  "anya-e1005n3:35": "Gabbhamassatarī yathā.",
  "anya-e1005n3:36": "86.",
  "anya-e1005n3:37": " Vahe a-mittaṃ khandhena,",
  "anya-e1005n3:38": "Yāva kālo anāgato;",
  "anya-e1005n3:39": "Tameva āgate kāle,",
  "anya-e1005n3:40": "Sele bhinde ghaṭaṃ iva.",
  "anya-e1005n3:41": "87.",
  "anya-e1005n3:42": " Iṇaseso aggiseso,",
  "anya-e1005n3:43": "Satruseso tatheva ca;",
  "anya-e1005n3:44": "Punappunaṃ vivaḍḍhanti,",
  "anya-e1005n3:45": "Tasmā sesaṃ na kāraye.",
  "anya-e1005n3:46": "88.",
  "anya-e1005n3:47": " Padumaṃva mukhaṃ yassa,",
  "anya-e1005n3:48": "Vācā candana sītalā;",
  "anya-e1005n3:49": "Tādisaṃ nopaseveyya,",
  "anya-e1005n3:50": "Hadayetu halāhalaṃ.",
  "anya-e1005n3:51": "89.",
  "anya-e1005n3:52": " Na seve pharusaṃ sāmiṃ,",
  "anya-e1005n3:53": "Naca seveyya macchariṃ;",
  "anya-e1005n3:54": "Tato apaggaṇhaṃ sāmiṃ,",
  "anya-e1005n3:55": "Neva niggahitaṃ tato.",
  "anya-e1005n3:56": "90.",
  "anya-e1005n3:57": " Siṅgī paññāsa hatthena,",
  "anya-e1005n3:58": "Vajje satena vājinaṃ;",
  "anya-e1005n3:59": "Hatthiṃ dantiṃ sahassena,",
  "anya-e1005n3:60": "Desa cāgena dujjanaṃ.",
  "anya-e1005n3:61": "91.",
  "anya-e1005n3:62": " Kudesañca kumittañca,",
  "anya-e1005n3:63": "Kukulañca kubandhavaṃ;",
  "anya-e1005n3:64": "Kudārañca kudāsañca,",
  "anya-e1005n3:65": "Dūrato parivajjaye.",
  "anya-e1005n3:66": "92.",
  "anya-e1005n3:67": " Rogāture ca dubbhikkhe,",
  "anya-e1005n3:68": "Byasane sattu viggahe;",
  "anya-e1005n3:69": "Rājadvāre susāne ca,",
  "anya-e1005n3:70": "Ye tiṭṭhanti sumittakā.",
  "anya-e1005n3:71": "93.",
  "anya-e1005n3:72": " Sītavāco bahumitto,",
  "anya-e1005n3:73": "Pharuso appamittako;",
  "anya-e1005n3:74": "Upamaṃ ettha ñātabbā,",
  "anya-e1005n3:75": "Canda-sūriya-rājūnaṃ.",
  "anya-e1005n3:76": "Mittakaṇḍo niṭṭhito."
}