{
  "anya-e0605n12:0": "Dvipādabyāsayamakagāthā",
  "anya-e0605n12:1": "87.",
  "anya-e0605n12:2": " Yasodharaṃ pīṇapayodharādharaṃ,",
  "anya-e0605n12:3": "Anaṅgaraṅgaddhajabhūtamaṅgaṃ;",
  "anya-e0605n12:4": "Devaccharāvujjalitaṃ patibbataṃ,",
  "anya-e0605n12:5": "Hitvā gato so sugato va nūna.",
  "anya-e0605n12:6": "88.",
  "anya-e0605n12:7": " Sabhāvanicchandamatiṃ pabhāvatiṃ,",
  "anya-e0605n12:8": "Bhatto kuso saṃhari bhattakājaṃ;",
  "anya-e0605n12:9": "Tāyābhirūpaṃ pi yasodharaṃ varaṃ,",
  "anya-e0605n12:10": "Hitvā gato so sugato va nūna.",
  "anya-e0605n12:11": "89.",
  "anya-e0605n12:12": " Pure pure saṇcari khaggahattho,",
  "anya-e0605n12:13": "Varaṃ paritthīnaṃ anitthigandho;",
  "anya-e0605n12:14": "Siriñca riñcāpi na riñci nāriṃ,",
  "anya-e0605n12:15": "Hitvānimandāni gato tathāgato.",
  "anya-e0605n12:16": "90.",
  "anya-e0605n12:17": " Harittaco rāgabalena deviyā,",
  "anya-e0605n12:18": "Avatthaliṅgena na liṅganussari;",
  "anya-e0605n12:19": "Asevi kāmaṃ tamidāni kāmaṃ,",
  "anya-e0605n12:20": "Hitvā gato so sugato va nūna.",
  "anya-e0605n12:21": "91.",
  "anya-e0605n12:22": " Apameyyakappesu vivekasevī,",
  "anya-e0605n12:23": "Hitvā gato rajjasiriṃ varitthiṃ;",
  "anya-e0605n12:24": "Aṇuṃ kaliṃ vaṇṇayi taṃ purāṇaṃ,",
  "anya-e0605n12:25": "Vatthamhi chiddaṃ viya tunnakāro.",
  "anya-e0605n12:26": "92.",
  "anya-e0605n12:27": " Tathāti mantvāna idāninaṅgo,",
  "anya-e0605n12:28": "Yasodharaṃ paggahito dhajaṃ va;",
  "anya-e0605n12:29": "Matto jitomhī ti pamattabandhu,",
  "anya-e0605n12:30": "Na passi ñāṇāsanipātamantaraṃ.",
  "anya-e0605n12:31": "93.",
  "anya-e0605n12:32": " Disvāna dukkhānalasambhavaṃbhavaṃ,",
  "anya-e0605n12:33": "Katvā taduppādakanaṅgabhaṅgaṃ;",
  "anya-e0605n12:34": "Yasodharaṃ pīṇapayodharādharaṃ,",
  "anya-e0605n12:35": "Hitvā gato buddhabalappadaṃ padaṃ.",
  "anya-e0605n12:36": "94.",
  "anya-e0605n12:37": " Anantasattānamanantakāle,",
  "anya-e0605n12:38": "Manaṅgahetvāna jito anaṅgo;",
  "anya-e0605n12:39": "Parājito nūna hi ekakassa,",
  "anya-e0605n12:40": "Tathāgato so na punāgato va.",
  "anya-e0605n12:41": "95.",
  "anya-e0605n12:42": " Disvāna ñāṇāsanipātamantaraṃ,",
  "anya-e0605n12:43": "Tathāgato so na punāgato va;",
  "anya-e0605n12:44": "Tathāgato so na punāgato va,",
  "anya-e0605n12:45": "Disvānañāṇāsanipātamantaraṃ."
}