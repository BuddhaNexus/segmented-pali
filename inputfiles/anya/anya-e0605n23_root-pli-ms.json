{
  "anya-e0605n23:0": "Abhisambodhidīpanīgāthā",
  "anya-e0605n23:1": "142.",
  "anya-e0605n23:2": " Purato gacchati cando rajatacakkaṁ va ambare,",
  "anya-e0605n23:3": "Sahassaraṁsi suriyo pacchimenupagacchati.",
  "anya-e0605n23:4": "143.",
  "anya-e0605n23:5": " Majjhe bodhidumacchatte pallaṅke apparājite,",
  "anya-e0605n23:6": "Pallaṅkena nisīditvā dhammaṁ sammasate muni.",
  "anya-e0605n23:7": "144.",
  "anya-e0605n23:8": " Sakko tasmiṁ khaṇe saṅkhaṁ dhamanto abhidhāvati,",
  "anya-e0605n23:9": "Brahmā tiyojanaṁ chattaṁ dhāreti munimuddhani.",
  "anya-e0605n23:10": "145.",
  "anya-e0605n23:11": " Maṇitālavaṇṭaṁ tusīto suyāmo vāḷabījaniṁ,",
  "anya-e0605n23:12": "Nānāmaṅgalabhaṇḍāni gahito sesadevatā.",
  "anya-e0605n23:13": "146.",
  "anya-e0605n23:14": " Evaṁ dasasahassamhi sakko brahmā ca devatā,",
  "anya-e0605n23:15": "Saṅkhādīnī dhamantā ca cakkavāḷamhi pūrayuṁ.",
  "anya-e0605n23:16": "147.",
  "anya-e0605n23:17": " Maṇgalāni gahetvāna tiṭṭhanti kāci devatā,",
  "anya-e0605n23:18": "Dhajamāla gahetvāna tathā puṇṇaghaṭādayo.",
  "anya-e0605n23:19": "148.",
  "anya-e0605n23:20": " Tattha naccanti gāyanti seḷenti vādayanti ca,",
  "anya-e0605n23:21": "Devā dasasahassamhi tuṭṭhahaṭṭhā pamoditā.",
  "anya-e0605n23:22": "149.",
  "anya-e0605n23:23": " Dhammāmatarasassādaṁ labhissāmassa santike,",
  "anya-e0605n23:24": "Nayanāmatarasassādaṁ pāṭihāriyañca passituṁ.",
  "anya-e0605n23:25": "150.",
  "anya-e0605n23:26": " Jāramaraṇakantārā sokopāyāsasallato,",
  "anya-e0605n23:27": "Mocesi kāmapāsamhā desento amataṁ padaṁ.",
  "anya-e0605n23:28": "151.",
  "anya-e0605n23:29": " Iti tuṭṭhehi devehi pūjiyanto narāsabho,",
  "anya-e0605n23:30": "Kiñci pūjaṁ acintento cintento dhammamuttamaṁ.",
  "anya-e0605n23:31": "152.",
  "anya-e0605n23:32": " Sabbatthasādhito santo siddhattho apparājito,",
  "anya-e0605n23:33": "Cakkavāḷasilāsāṇipākārehi manorame.",
  "anya-e0605n23:34": "153.",
  "anya-e0605n23:35": " Tārāmaṇikhacitākāsavitāne candadīpake,",
  "anya-e0605n23:36": "Mānāratamapajjote mālāgandhādipūjite.",
  "anya-e0605n23:37": "154.",
  "anya-e0605n23:38": " Dibbehi chaṇabherīhi ghuṭṭhe maṅgalagītiyā,",
  "anya-e0605n23:39": "Cakkavāḷe suppāsāde bodhimaṇḍamahātale.",
  "anya-e0605n23:40": "155.",
  "anya-e0605n23:41": " Bodhirukkhamaṇicchatte pallaṅke apparājite,",
  "anya-e0605n23:42": "Nissinno paṭhame yāme purimaṁ jātimanussari.",
  "anya-e0605n23:43": "156.",
  "anya-e0605n23:44": " Namarūpāmanuppatti sudiṭṭhā hoti tenidhā,",
  "anya-e0605n23:45": "Sakkātadiṭṭhi tenassa pahīnā hoti sabbaso.",
  "anya-e0605n23:46": "157.",
  "anya-e0605n23:47": " Tato hi dutiye yāme yathāyammupage sari,",
  "anya-e0605n23:48": "Sudiṭṭhaṁ hoti tenassa kammakklesehi sambhavaṁ.",
  "anya-e0605n23:49": "158.",
  "anya-e0605n23:50": " Kaṅkhāvitaraṇī nāma ñāṇantaṁ samupāgataṁ,",
  "anya-e0605n23:51": "Tenasesa pahīyittha kaṅkhā soḷasadhā ṭhitā.",
  "anya-e0605n23:52": "159.",
  "anya-e0605n23:53": " Tato so tatiye yāme dvādasaṅge asesato,",
  "anya-e0605n23:54": "So paṭiccasamuppāde ñāṇamotārayī muni.",
  "anya-e0605n23:55": "160.",
  "anya-e0605n23:56": " Avijjavādyānulomena jarādipaṭilomato,",
  "anya-e0605n23:57": "Sammasanto yathābhūtaṁ ñāṇadassanamāgami.",
  "anya-e0605n23:58": "161.",
  "anya-e0605n23:59": " Kappakoṭisatenāpi appameyyesu jātisu,",
  "anya-e0605n23:60": "Lobhaṁ asesadānena vināsento punappunaṁ.",
  "anya-e0605n23:61": "162.",
  "anya-e0605n23:62": " Sīlena khantimettāya kokhadosaṁ nivāresi,",
  "anya-e0605n23:63": "Paññāya mohaṁ chetvāna micchādiṭṭhi tatheva ca.",
  "anya-e0605n23:64": "163.",
  "anya-e0605n23:65": " Garūpasevanādīhi vicikicchaṁ vinodayaṁ,",
  "anya-e0605n23:66": "Mānuddhaccaṁ vinodento kule jeṭṭhopacāyinā.",
  "anya-e0605n23:67": "164.",
  "anya-e0605n23:68": " Nekkhammena vināsento kāmarāgaṁ punappunaṁ,",
  "anya-e0605n23:69": "Saccena visaṁvādaṁ kosajjaṁ vīriyena ca.",
  "anya-e0605n23:70": "165.",
  "anya-e0605n23:71": " Evaṁ dānādinā taṁ taṁ kilesaṇgaṁ vinodayaṁ,",
  "anya-e0605n23:72": "Suvaḍḍhitā mahāpaññā kathaṁ santiṁ na rūhati.",
  "anya-e0605n23:73": "166.",
  "anya-e0605n23:74": " Sudukkaraṁ karitvāna dānādipaccayaṁ pure,",
  "anya-e0605n23:75": "Na kiñci bhavasampattiṁ patthesi bodhimuttamaṁ.",
  "anya-e0605n23:76": "167.",
  "anya-e0605n23:77": " Paṇidhānamhā paṭṭhāya kataṁ puññañca patthanaṁ,",
  "anya-e0605n23:78": "Ekkattha dāni sampattiṁ deti bodhiṁ asaṁsayaṁ.",
  "anya-e0605n23:79": "168.",
  "anya-e0605n23:80": " Tato so sabbasaṅkhāre aniccadukkhanattato,",
  "anya-e0605n23:81": "Sammasantonulomena nibbānaṁ samupāgami.",
  "anya-e0605n23:82": "169.",
  "anya-e0605n23:83": " Savāsane kilese so jhāpentonumattaṁ pi ca,",
  "anya-e0605n23:84": "Arahattappattiyā suddho buddho bodhitale ahu.",
  "anya-e0605n23:85": "170.",
  "anya-e0605n23:86": " Patto vimettiṁ varasetachattaṁ,",
  "anya-e0605n23:87": "So pītivegena udānudīrayi;",
  "anya-e0605n23:88": "Chetvāna māre vijitārisaṅgho,",
  "anya-e0605n23:89": "Tibuddhakhettekadivākaro ahu.",
  "anya-e0605n23:90": "171.",
  "anya-e0605n23:91": " Rājādhirājā varamevamāsi,",
  "anya-e0605n23:92": "Tichattadhāri varadhammarājā;",
  "anya-e0605n23:93": "Mahāsahassaṁ pi ca lokadhātuṁ,",
  "anya-e0605n23:94": "Sarena viññāpayituṁ samattho.",
  "anya-e0605n23:95": "172.",
  "anya-e0605n23:96": " Buddho lokāloke loke,",
  "anya-e0605n23:97": "Jāto satto konummatto;",
  "anya-e0605n23:98": "Suddhaṁ buddhaṁ oghā tiṇṇaṁ,",
  "anya-e0605n23:99": "Saddho pañño ko no vande.",
  "anya-e0605n23:100": "173.",
  "anya-e0605n23:101": " Bhajitaṁ cajitaṁ pavanaṁ bhavanaṁ,",
  "anya-e0605n23:102": "Jahitaṁ gahitaṁ samalaṁ amalaṁ;",
  "anya-e0605n23:103": "Sugataṁ agataṁ sugatiṁ agatiṁ,",
  "anya-e0605n23:104": "Namitaṁ amitaṁ namatiṁ sumatiṁ."
}