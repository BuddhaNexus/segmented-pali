{
  "anya-e1002n:6": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1002n:7": "Nītimañjarī",
  "anya-e1002n:8": "1.",
  "anya-e1002n:9": "Kulakkhaye vinassanti,",
  "anya-e1002n:10": "Kuladhammā sanantanā;",
  "anya-e1002n:11": "Dhamme naṭṭhe kulaṃ sabbaṃ,",
  "anya-e1002n:12": "Adhammo abhibhū khalaṃ.",
  "anya-e1002n:13": "2.",
  "anya-e1002n:14": "Adhammābhibhavā dantā,",
  "anya-e1002n:15": "Padussanti kulitthiyo;",
  "anya-e1002n:16": "Thīsu duṭṭhā sva dhammena,",
  "anya-e1002n:17": "Jāyate vaṇṇasaṅkaro.",
  "anya-e1002n:18": "3.",
  "anya-e1002n:19": "Piyaṃ bhāse guṇaggāho,",
  "anya-e1002n:20": "Sūro siyā vikantano;",
  "anya-e1002n:21": "Dātā candasamā nārī,",
  "anya-e1002n:22": "Diṭṭhaṃ diṭṭhaṃ nahāsaye.",
  "anya-e1002n:23": "4.",
  "anya-e1002n:24": "Kutotthi kumitte saccaṃ,",
  "anya-e1002n:25": "Kudāre rativaḍḍhanaṃ;",
  "anya-e1002n:26": "Kudesamhi mano rammaṃ,",
  "anya-e1002n:27": "Kurāje bhogasampadaṃ.",
  "anya-e1002n:28": "Saṅketeva amittasmiṃ,",
  "anya-e1002n:29": "Mittasmiṃ pi navissase;",
  "anya-e1002n:30": "Abhayā bhaya muppannaṃ,",
  "anya-e1002n:31": "Api mūlāni kantati.",
  "anya-e1002n:32": "Adiṭṭhova paro seyyo,",
  "anya-e1002n:33": "Dummitto no vissāsiko.",
  "anya-e1002n:34": "Aggihomaphalaṃ vedo,",
  "anya-e1002n:35": "Satthaṃsīlaphalaṃ mataṃ;",
  "anya-e1002n:36": "Ratiputtaphalaṃ nārī,",
  "anya-e1002n:37": "Dānabhuttiphalaṃ dhanaṃ.",
  "anya-e1002n:38": "Asaccaṃ sāhasaṃ māyā,",
  "anya-e1002n:39": "Mūḷhatta ma tilobhatā;",
  "anya-e1002n:40": "Asocaṃ niddayattañca,",
  "anya-e1002n:41": "Thīnaṃ dosā sabhāvajā.",
  "anya-e1002n:42": "Jāyāya bhattuno bhāro,",
  "anya-e1002n:43": "Sissena guruno kato;",
  "anya-e1002n:44": "Amaccakehi rājassa,",
  "anya-e1002n:45": "Pitarānaṃ nijenaca.",
  "anya-e1002n:46": "5.",
  "anya-e1002n:47": "Uyyamena hi sijjhanti,",
  "anya-e1002n:48": "Kammāni na manorathā;",
  "anya-e1002n:49": "Na hi suttassa sīhassa,",
  "anya-e1002n:50": "Pavīsanti mukhe migā.",
  "anya-e1002n:51": "Atisītaṃ atiuṇhaṃ,",
  "anya-e1002n:52": "Atisāyamidaṃ ahu;",
  "anya-e1002n:53": "Iti visaṭṭhakammante,",
  "anya-e1002n:54": "Khaṇā accenti māṇave.",
  "anya-e1002n:55": "Ādānassa padānassa,",
  "anya-e1002n:56": "Kattabbassa ca kammuno;",
  "anya-e1002n:57": "Khippaṃ akayyamānassa,",
  "anya-e1002n:58": "Kāle pivati sampadaṃ.",
  "anya-e1002n:59": "Nādabbe nihitā kāci,",
  "anya-e1002n:60": "Kriyā phalavatī bhave;",
  "anya-e1002n:61": "Nabyāpārasatenāpi,",
  "anya-e1002n:62": "Sukova pāṭhate bako.",
  "anya-e1002n:63": "Yo dandhakāle tarati,",
  "anya-e1002n:64": "Taraṇīye ca dandhaye;",
  "anya-e1002n:65": "Sukkhapaṇṇaṃ va akkamma,",
  "anya-e1002n:66": "Atthaṃ bhañjati attano.",
  "anya-e1002n:67": "6.",
  "anya-e1002n:68": "Yaṃ dadāti yaṃ bhuñjati,",
  "anya-e1002n:69": "Tadeva dhanino dhanaṃ;",
  "anya-e1002n:70": "Aññe matassa kīḷanti,",
  "anya-e1002n:71": "Dārehipi dhanehipi.",
  "anya-e1002n:72": "Dānopabhogahīnena,",
  "anya-e1002n:73": "Dhanena dhanino sukhaṃ;",
  "anya-e1002n:74": "Ko viseso daliddassa,",
  "anya-e1002n:75": "Adhikaṃ dhanarakkhaṇaṃ.",
  "anya-e1002n:76": "Nijasokhyaṃ nirundhanto,",
  "anya-e1002n:77": "Nīcabhogo mitampaco;",
  "anya-e1002n:78": "Dhanaṃ sañcayate yo so,",
  "anya-e1002n:79": "Parabhāravaho pasu.",
  "anya-e1002n:80": "Yaṃ ussukā saṅkharonti,",
  "anya-e1002n:81": "Alakkhikā bahuṃ dhanaṃ;",
  "anya-e1002n:82": "Sippavanto asippāvā,",
  "anya-e1002n:83": "Lakkhi vā tāni bhuñjati.",
  "anya-e1002n:84": "7.",
  "anya-e1002n:85": "Sampatyaṃ mahataṃ cittaṃ,",
  "anya-e1002n:86": "Bhave uppale komalaṃ;",
  "anya-e1002n:87": "Vipatyaṃca mahāsela,",
  "anya-e1002n:88": "Silāsaṅghātakakkasaṃ.",
  "anya-e1002n:89": "8.",
  "anya-e1002n:90": "Asambhabyaguṇaṃ thutvā,",
  "anya-e1002n:91": "Khedo mudhāva jāyate;",
  "anya-e1002n:92": "Avhāyaṃ canda mu llokya,",
  "anya-e1002n:93": "Nacandota mu pāgamī.",
  "anya-e1002n:94": "9.",
  "anya-e1002n:95": "Saccaṃ mukhamhi dhāreyya,",
  "anya-e1002n:96": "Kaṇṇe sutaṃ bhuje jayaṃ;",
  "anya-e1002n:97": "Hadayamhi khamaṃ vīraṃ,",
  "anya-e1002n:98": "Lokādāsaṃca locane.",
  "anya-e1002n:99": "Saddamattaṃ naphandeyya,",
  "anya-e1002n:100": "Aññatvā saddakāraṇaṃ;",
  "anya-e1002n:101": "Saddahetuṃ pariññāya,",
  "anya-e1002n:102": "Pamodo vā bhayo tathā.",
  "anya-e1002n:103": "Sabbasuta ma dhīyeyya,",
  "anya-e1002n:104": "Hīnamukkaṭṭhamajjhimaṃ.",
  "anya-e1002n:105": "10.",
  "anya-e1002n:106": "Dunnāriyā kulaṃ suddhaṃ,",
  "anya-e1002n:107": "Putto nassati lālanā;",
  "anya-e1002n:108": "Samiddhi anayā bandhu,",
  "anya-e1002n:109": "Pavāsā madanā hirī.",
  "anya-e1002n:110": "Lālaye pañcavassāni,",
  "anya-e1002n:111": "Dasavassāni tālaye;",
  "anya-e1002n:112": "Pattetu soḷasevasse,",
  "anya-e1002n:113": "Puttaṃ mittaṃva ācare.",
  "anya-e1002n:114": "Lālane bahavo dosā,",
  "anya-e1002n:115": "Lālane bahavo guṇā.",
  "anya-e1002n:116": "Pāpā nivārayati yojayate hitāya,",
  "anya-e1002n:117": "Guyhāni gūhati guṇaṃ pakaṭīkaroti;",
  "anya-e1002n:118": "Āpattikañca najahāti dadāti kāle,",
  "anya-e1002n:119": "Sammitta lakkhaṇamidaṃ pavadanti santo.",
  "anya-e1002n:120": "11.",
  "anya-e1002n:121": "Dujjano jīyate yutyā,",
  "anya-e1002n:122": "Niggahena nadhīmatā;",
  "anya-e1002n:123": "Nipātyate mahārukkho,",
  "anya-e1002n:124": "Tassamīpa khatikkhayā.",
  "anya-e1002n:125": "Vane migāca luddhānaṃ,",
  "anya-e1002n:126": "Dujjanānañca sajjanā;",
  "anya-e1002n:127": "Akāraṇaverī honti,",
  "anya-e1002n:128": "Tiṇabhakkhā supesalā.",
  "anya-e1002n:129": "Pādalaggaṃ karaṭṭhena,",
  "anya-e1002n:130": "Kaṇḍakeneva kaṇḍakaṃ.",
  "anya-e1002n:131": "Bālaṃ napasse nasuṇe,",
  "anya-e1002n:132": "Nacabālena saṃvase;",
  "anya-e1002n:133": "Bālenāllāpasallāpaṃ,",
  "anya-e1002n:134": "Nakare nacarocaye.",
  "anya-e1002n:135": "12.",
  "anya-e1002n:136": "Upa kattuṃ yathā khuddo,",
  "anya-e1002n:137": "Samattho natathāmahā;",
  "anya-e1002n:138": "Kūpo hi hanti pipāsaṃ,",
  "anya-e1002n:139": "Natu pāyo mahambudhi.",
  "anya-e1002n:140": "13.",
  "anya-e1002n:141": "Ādānassa padānassa,",
  "anya-e1002n:142": "Kattabbassaca kammuno;",
  "anya-e1002n:143": "Khippaṃ akaramānassa,",
  "anya-e1002n:144": "Kālo bhakkhati taṃ rasaṃ.",
  "anya-e1002n:145": "Nakkhattaṃ paṭimānentaṃ,",
  "anya-e1002n:146": "Attho bālaṃ upajjhagā;",
  "anya-e1002n:147": "Attho atthassa nakkhattaṃ,",
  "anya-e1002n:148": "Kiṃ karissanti tārakā.",
  "anya-e1002n:149": "Ajarāmarova pañño,",
  "anya-e1002n:150": "Vijjamatthañca cintaye;",
  "anya-e1002n:151": "Gahitoviya kesesu,",
  "anya-e1002n:152": "Maccunā dhammamācare.",
  "anya-e1002n:153": "14.",
  "anya-e1002n:154": "Vajjā gurūca mantīca,",
  "anya-e1002n:155": "Tayo raṭṭhābhisaṅkhatā;",
  "anya-e1002n:156": "Jīvīta dakkha kosānaṃ,",
  "anya-e1002n:157": "Vaḍḍhanā nāsanāca te.",
  "anya-e1002n:158": "15.",
  "anya-e1002n:159": "Thirena kammaṃ vaḍḍhati,",
  "anya-e1002n:160": "Athirena turena no;",
  "anya-e1002n:161": "Phalanti samaye rukkhā,",
  "anya-e1002n:162": "Sittāpi bahuvārinā.",
  "anya-e1002n:163": "Vāyāmetheva puriso,",
  "anya-e1002n:164": "Nanibbindeyya paṇḍito.",
  "anya-e1002n:165": "Payatano tādiso neva,",
  "anya-e1002n:166": "Kayyo yena phalaṃ nahi;",
  "anya-e1002n:167": "Selagge kūpakhaṇanā,",
  "anya-e1002n:168": "Kathaṃ toyasamāgamo.",
  "anya-e1002n:169": "Ñāṇaṅkusena sammaggaṃ,",
  "anya-e1002n:170": "Niyyatyussāhakuñjaro.",
  "anya-e1002n:171": "Asamekkhitakammantaṃ,",
  "anya-e1002n:172": "Turitābhi nipātinaṃ;",
  "anya-e1002n:173": "Tānikammāni tappenti,",
  "anya-e1002n:174": "Uṇhaṃ va jjhohitaṃ mukhe.",
  "anya-e1002n:175": "16.",
  "anya-e1002n:176": "Chaddosā puriseneha,",
  "anya-e1002n:177": "Hātabbā bhūtimicchantā;",
  "anya-e1002n:178": "Niddā majjaṃ bhayaṃ kodho,",
  "anya-e1002n:179": "Ālasyaṃ dīghasuttatā.",
  "anya-e1002n:180": "Na divā suppasīlena,",
  "anya-e1002n:181": "Rattimuṭṭhānadessinā;",
  "anya-e1002n:182": "Niccasoṇḍena mattena,",
  "anya-e1002n:183": "Sakkā āvasituṃ gharaṃ.",
  "anya-e1002n:184": "Abhetabbamhi bhāyanti,",
  "anya-e1002n:185": "Bhāyitabbe nabhāyare;",
  "anya-e1002n:186": "Bhayābhaya vimuḷhā te,",
  "anya-e1002n:187": "Jimhānugā ujuñjahā.",
  "anya-e1002n:188": "Yassa manussabhūtassa,",
  "anya-e1002n:189": "Natthi bhogāca sippakaṃ;",
  "anya-e1002n:190": "Kiṃ phalaṃ tassa mānussaṃ,",
  "anya-e1002n:191": "Dvipādaṭṭho hi so migo.",
  "anya-e1002n:192": "17.",
  "anya-e1002n:193": "Nānopāyova kattabbo,",
  "anya-e1002n:194": "Sace bhaveyya attano;",
  "anya-e1002n:195": "Atthasiddhi yathākāmaṃ,",
  "anya-e1002n:196": "Upāyo hi hitañjaso.",
  "anya-e1002n:197": "Lañjadānabālisena,",
  "anya-e1002n:198": "Kūṭaḍḍakāradhīvarā;",
  "anya-e1002n:199": "Vinicchayamahāmacchaṃ,",
  "anya-e1002n:200": "Oṭṭenti lobhasāgare.",
  "anya-e1002n:201": "Yassete caturo dhammā,",
  "anya-e1002n:202": "Vānarinda yathātava;",
  "anya-e1002n:203": "Saccaṃ dhammo dhīti cāgo,",
  "anya-e1002n:204": "Diṭṭhaṃ so ativattati.",
  "anya-e1002n:205": "18.",
  "anya-e1002n:206": "Vidvāca ratanaṃ nārī,",
  "anya-e1002n:207": "Vīṇā sātthaṃ giraṃmahī;",
  "anya-e1002n:208": "Guṇavisesa māgamma,",
  "anya-e1002n:209": "Guṇāni aguṇānica.",
  "anya-e1002n:210": "Dhanavā balavā loke,",
  "anya-e1002n:211": "Dhanā bhavati paṇḍito.",
  "anya-e1002n:212": "Sumane nissito kīṭo,",
  "anya-e1002n:213": "Nigguṇo hīnako sayaṃ;",
  "anya-e1002n:214": "Taṃ pupphehi maṇḍentānaṃ,",
  "anya-e1002n:215": "Raññaṃ siropi rohati.",
  "anya-e1002n:216": "Alakkhikehi sañcītā,",
  "anya-e1002n:217": "Dhanabhogāca cintitā;",
  "anya-e1002n:218": "Lakkhikassa bhavantete,",
  "anya-e1002n:219": "Lakkhivā suṭṭhubhuñjati.",
  "anya-e1002n:220": "Khattiyo seṭṭho jane tasmiṃ,",
  "anya-e1002n:221": "Yo gottapaṭisārino;",
  "anya-e1002n:222": "Vijjācaraṇasampanno,",
  "anya-e1002n:223": "So seṭṭho devamānuse.",
  "anya-e1002n:224": "Visāpi amataṃ gaṇhe,",
  "anya-e1002n:225": "Gūthato maṇimuttamaṃ;",
  "anya-e1002n:226": "Kaṇṭakapādapā pupphaṃ,",
  "anya-e1002n:227": "Thirataṃ dukkulā varaṃ.",
  "anya-e1002n:228": "Dhanissarādiguṇommi -",
  "anya-e1002n:229": "Vegena vāhitā pajā.",
  "anya-e1002n:230": "19.",
  "anya-e1002n:231": "Yassa tthi satataṃ mettā,",
  "anya-e1002n:232": "Sabbalokasuvallabhā;",
  "anya-e1002n:233": "Kūpāyate samuddopi,",
  "anya-e1002n:234": "Aggi tassa jalāyate.",
  "anya-e1002n:235": "20.",
  "anya-e1002n:236": "Sakkharāyati merūpi,",
  "anya-e1002n:237": "Visabhakkho sudhāyate;",
  "anya-e1002n:238": "Sasāyate migarāja,",
  "anya-e1002n:239": "Byālo mālāguṇāyate;",
  "anya-e1002n:240": "Dolāyate chamācālo,",
  "anya-e1002n:241": "Nānāvudhā tiṇāyare.",
  "anya-e1002n:242": "21.",
  "anya-e1002n:243": "Sameva sati ussāhe,",
  "anya-e1002n:244": "Sukhavāho hitaṅkaro;",
  "anya-e1002n:245": "Ūne-dhike tathā nohi,",
  "anya-e1002n:246": "Majjhago sādhu sabbadā.",
  "anya-e1002n:247": "Sādhu kho paṇḍitonāma,",
  "anya-e1002n:248": "Natveva atipaṇḍito."
}