{
  "anya-e1007n:0": " Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1007n:1": "Sūrassatīnīti",
  "anya-e1007n:2": "Paṭhamo bhāgo",
  "anya-e1007n:3": "Paṇāmagāthā",
  "anya-e1007n:4": "1.",
  "anya-e1007n:5": " Mukhamhā bhagavantassa,",
  "anya-e1007n:6": "Sugandhakamalā subhā;",
  "anya-e1007n:7": "Sañjātaṃ uttamaṃ vāṇiṃ,",
  "anya-e1007n:8": "Vandāmi vittamānasā.",
  "anya-e1007n:9": "2.",
  "anya-e1007n:10": " Samajjhiṭṭho racissāmi,",
  "anya-e1007n:11": "Dakkhena rājamantinā;",
  "anya-e1007n:12": "Nāmena santena,",
  "anya-e1007n:13": "Nītiṃ lokahitāvahaṃ;",
  "anya-e1007n:14": "Sālaṅkāraṃ sopadesaṃ,",
  "anya-e1007n:15": "Nāmenāhaṃ svarassatiṃ.",
  "anya-e1007n:16": "3.",
  "anya-e1007n:17": " Pekkhantvimaṃ nītiṃ santā,",
  "anya-e1007n:18": "Mañjūsarukkhasannibhaṃ;",
  "anya-e1007n:19": "Nānopadesasaṃpuṇṇaṃ,",
  "anya-e1007n:20": "Sadatthakusalā sadā.",
  "anya-e1007n:21": "Sūrassatīnīti",
  "anya-e1007n:22": "1.",
  "anya-e1007n:23": " Kataññutā ca saccañca,",
  "anya-e1007n:24": "Lokasārā hi te duve;",
  "anya-e1007n:25": "Lokāpi tehi tiṭṭhanti,",
  "anya-e1007n:26": "Raṭṭhaṃ akaṃsu issaraṃ.",
  "anya-e1007n:27": "2.",
  "anya-e1007n:28": " Kākāca dujjanā loke,",
  "anya-e1007n:29": "Malībhūtāva sabbadā;",
  "anya-e1007n:30": "Iṭṭhaṃ guṇaṃ nāsayanti,",
  "anya-e1007n:31": "Te ve lokassa verino.",
  "anya-e1007n:32": "Migānaṃ siṅgālo anto,",
  "anya-e1007n:33": "Pakkhīnaṃ pana vāyaso.",
  "anya-e1007n:34": "Akāraṇaverī honti,",
  "anya-e1007n:35": "Macchānaṃ dhīvarā yathā;",
  "anya-e1007n:36": "Guṇīnaṃ sajjanānañca,",
  "anya-e1007n:37": "Dujjanā niccaverino.",
  "anya-e1007n:38": "3.",
  "anya-e1007n:39": " Siriṃ bhonto suposetha,",
  "anya-e1007n:40": "Siri mūlā hi sampadā;",
  "anya-e1007n:41": "Siriya idha jotanti,",
  "anya-e1007n:42": "Sirī sā sabbasiddhikā.",
  "anya-e1007n:43": "4.",
  "anya-e1007n:44": " Musā tamokarā loke,",
  "anya-e1007n:45": "Saccaṃ mīditikārakaṃ;",
  "anya-e1007n:46": "Musātamena dukkhanti,",
  "anya-e1007n:47": "Saccābhāya sukhantive.",
  "anya-e1007n:48": "Suvijānaṃ siṅgālānaṃ,",
  "anya-e1007n:49": "Sakuṇānañca vassitaṃ;",
  "anya-e1007n:50": "Manussavassitaṃ rāja,",
  "anya-e1007n:51": "Dubbijānataraṃ tato.",
  "anya-e1007n:52": "5.",
  "anya-e1007n:53": " Kappaggisadisā issā,",
  "anya-e1007n:54": "Jhāpeti sabbasampadaṃ;",
  "anya-e1007n:55": "Muditā kappameghova,",
  "anya-e1007n:56": "Ropeti sabbasampadaṃ.",
  "anya-e1007n:57": "6.",
  "anya-e1007n:58": " Yathā asanthirā thambhā,",
  "anya-e1007n:59": "Thusarāsimhi ussitā;",
  "anya-e1007n:60": "Tatheva kapicittānaṃ,",
  "anya-e1007n:61": "Kammantā cañcalaṅgatā.",
  "anya-e1007n:62": "Haliddirāgaṃ kapicittaṃ;",
  "anya-e1007n:63": "Purisaṃ rāgavirāginaṃ,",
  "anya-e1007n:64": "Edisaṃ tāta māsevi;",
  "anya-e1007n:65": "Nimmanussampi ce siyā.",
  "anya-e1007n:66": "7. [Ka]",
  "anya-e1007n:67": " Pupphacumbi cittapattī,",
  "anya-e1007n:68": "Sakavaṇṇena majjito;",
  "anya-e1007n:69": "Vikkamī pupphato pupphaṃ,",
  "anya-e1007n:70": "Nassāgāraṃ na saṅgamo.",
  "anya-e1007n:71": "[Kha]",
  "anya-e1007n:72": "Kippilikā dubbaṇṇāpi,",
  "anya-e1007n:73": "Samaggāca parakkamā;",
  "anya-e1007n:74": "Mā hotha pupphacumbīva,",
  "anya-e1007n:75": "Hotha vo pacikā yathā.",
  "anya-e1007n:76": "1.",
  "anya-e1007n:77": " Ye maranti kīḷantā te,",
  "anya-e1007n:78": "Svānā aññoññamoditā;",
  "anya-e1007n:79": "Disvāna chaṭṭitaṃ bhattaṃ,",
  "anya-e1007n:80": "Sīghaverī vihiṃsare.",
  "anya-e1007n:81": "2.",
  "anya-e1007n:82": " Tathekepi janā dāni,",
  "anya-e1007n:83": "Samaggā ññoññamoditā;",
  "anya-e1007n:84": "Dhanahetu vihiṃsanti,",
  "anya-e1007n:85": "Dhīratthu sīghaverikā.",
  "anya-e1007n:86": "9.",
  "anya-e1007n:87": " Vasante hemante gimhe;",
  "anya-e1007n:88": "Neva tālā visesino;",
  "anya-e1007n:89": "Thiracittā janā santā;",
  "anya-e1007n:90": "Sukhadukkhesu niccalā.",
  "anya-e1007n:91": "10.",
  "anya-e1007n:92": " Yathā pavaṭṭamānamhi,",
  "anya-e1007n:93": "Suṭṭhu tiṭṭhati geṇṭhuke;",
  "anya-e1007n:94": "Appavaṭṭe bhūmyaṃ seti,",
  "anya-e1007n:95": "Tatheva geṇṭhuko jano.",
  "anya-e1007n:96": "11.",
  "anya-e1007n:97": " Agghāpetuṃ nasakkonti,",
  "anya-e1007n:98": "Kālañhi kālikā janā;",
  "anya-e1007n:99": "Vajirādiñca sakkonti,",
  "anya-e1007n:100": "Tena kālo anagghiko.",
  "anya-e1007n:101": "12. [Ka]",
  "anya-e1007n:102": " Asano hi dīghaddhāno,",
  "anya-e1007n:103": "Sārasāro sugandhiko;",
  "anya-e1007n:104": "Niggandho tveva nissāro,",
  "anya-e1007n:105": "Dīghaddhānopi simbalī.",
  "anya-e1007n:106": "[Kha]",
  "anya-e1007n:107": "Tatheveke janā loke,",
  "anya-e1007n:108": "Dīghaddhānā susārakā;",
  "anya-e1007n:109": "Nissārā keci pheggūva,",
  "anya-e1007n:110": "Dīghaddhānāpi goyathā.",
  "anya-e1007n:111": "13.",
  "anya-e1007n:112": " Upakāro cāpakāro,",
  "anya-e1007n:113": "Yasmiṃ gacchati naṭṭhataṃ;",
  "anya-e1007n:114": "Pāsāṇahadayassassa,",
  "anya-e1007n:115": "Jīvatītyā bhidhāmudhā.",
  "anya-e1007n:116": "Pasādo nipphalo yassa,",
  "anya-e1007n:117": "Kopocāpi niratthako;",
  "anya-e1007n:118": "Na taṃ saṅgantu miccheyya,",
  "anya-e1007n:119": "Thīpumāva napuṃsakaṃ.",
  "anya-e1007n:120": "14.",
  "anya-e1007n:121": " Upacāro hi kātabbo,",
  "anya-e1007n:122": "Na yāva sohadaṃ bhave;",
  "anya-e1007n:123": "Upacāro sumittamhi,",
  "anya-e1007n:124": "Māyā ca hoti koṭilaṃ.",
  "anya-e1007n:125": "15.",
  "anya-e1007n:126": " Kamena aggato ucchu,",
  "anya-e1007n:127": "Raso sādutaro yathā;",
  "anya-e1007n:128": "Tatheva sumitto loke,",
  "anya-e1007n:129": "Dummitto pana nediso.",
  "anya-e1007n:130": "16.",
  "anya-e1007n:131": " Sokārāti parittāṇaṃ,",
  "anya-e1007n:132": "Vissāsapītibhājanaṃ;",
  "anya-e1007n:133": "Ratanābhiratanaṃ icche,",
  "anya-e1007n:134": "Sumittaṃ akkharattayaṃ.",
  "anya-e1007n:135": "17.",
  "anya-e1007n:136": " Dampatīnaṃ sumittānaṃ,",
  "anya-e1007n:137": "Mukhaṃ aññoññadappaṇaṃ,",
  "anya-e1007n:138": "Sukhe sukhaṃ dukkhe dukkhaṃ,",
  "anya-e1007n:139": "Paṭicchāyeva dappaṇe.",
  "anya-e1007n:140": "Kathaṃ nu tāsaṃ hadayaṃ,",
  "anya-e1007n:141": "Sukharāvata itthiyo;",
  "anya-e1007n:142": "Yāsāmike dukkhitamhi,",
  "anya-e1007n:143": "Sukhamicchanti attano.",
  "anya-e1007n:144": "18.",
  "anya-e1007n:145": " Nivātañca pure katvā,",
  "anya-e1007n:146": "Mānaṃ katvāna pacchato;",
  "anya-e1007n:147": "Sakatthaṃ dhāraye dhīro,",
  "anya-e1007n:148": "Atthabhañjo hi muḷhatā.",
  "anya-e1007n:149": "19.",
  "anya-e1007n:150": " Purecāraṃ satiṃ katvā,",
  "anya-e1007n:151": "Saddhaṃ kareyya pacchato;",
  "anya-e1007n:152": "Turaṃ na saddahe dhīro,",
  "anya-e1007n:153": "Sīghasaddho hi mandako.",
  "anya-e1007n:154": "20.",
  "anya-e1007n:155": " Vane 0.0099 bahūni kaṭṭhāni;",
  "anya-e1007n:156": "Dullabhaṃ rattacandanaṃ;",
  "anya-e1007n:157": "Tathā janā bahū loke;",
  "anya-e1007n:158": "Pumā jañño sudullabho.",
  "anya-e1007n:159": "21.",
  "anya-e1007n:160": " Tiṇakaṭṭhapalāsehi;",
  "anya-e1007n:161": "Sukkhehi dayhate vanaṃ;",
  "anya-e1007n:162": "Etādīhi asārehi;",
  "anya-e1007n:163": "Loko janehi dayhate.",
  "anya-e1007n:164": "22.",
  "anya-e1007n:165": " Antovasse timāsamhi,",
  "anya-e1007n:166": "Puññakammena moditā;",
  "anya-e1007n:167": "Sukhaṃ vasiṃsu porāṇā,",
  "anya-e1007n:168": "Buddhasāsanamāmakā.",
  "anya-e1007n:169": "23.",
  "anya-e1007n:170": " Migamadena ekena,",
  "anya-e1007n:171": "Taṃ vanaṃ surabhigandhikaṃ;",
  "anya-e1007n:172": "Tathā janena taṃ raṭṭhaṃ,",
  "anya-e1007n:173": "Guṇinā hi sirīmatā.",
  "anya-e1007n:174": "Sarīraṃ khaṇaviddhaṃsī,",
  "anya-e1007n:175": "Kappantaṭṭhāyino guṇā.",
  "anya-e1007n:176": "24.",
  "anya-e1007n:177": " Kharānaṃ sīhabyagghānaṃ,",
  "anya-e1007n:178": "Saṅgamo no hisabbadā;",
  "anya-e1007n:179": "Tatheva byagghacittānaṃ,",
  "anya-e1007n:180": "Sajātikā khayonatā.",
  "anya-e1007n:181": "25.",
  "anya-e1007n:182": " Saka sādhupi no sādhū,",
  "anya-e1007n:183": "Yo ceñña duṭṭhakārako;",
  "anya-e1007n:184": "Bahūnaṃ sādhū pāyena,",
  "anya-e1007n:185": "Sa ve sādhūti vuccate.",
  "anya-e1007n:186": "26.",
  "anya-e1007n:187": " Bahūdake samuddepi,",
  "anya-e1007n:188": "Jalaṃ nattheva pātave;",
  "anya-e1007n:189": "Khuddake khatakūpamhi,",
  "anya-e1007n:190": "Sāduṃ atthi bahuṃ dakaṃ.",
  "anya-e1007n:191": "27.",
  "anya-e1007n:192": " Mā sīghaṃ vivareyyātha,",
  "anya-e1007n:193": "Nindituñca pasaṃsituṃ;",
  "anya-e1007n:194": "Mukhañhi vo kathādvāraṃ,",
  "anya-e1007n:195": "Nirundheyyātha sabbadā.",
  "anya-e1007n:196": "28.",
  "anya-e1007n:197": " Mā sīghaṃ vivareyyātha,",
  "anya-e1007n:198": "Cakkhuṃ vo dassituṃ piyaṃ;",
  "anya-e1007n:199": "Saṇikañhi piyalābhaṃ,",
  "anya-e1007n:200": "Dhanalābhaṃ turaṃ kare.",
  "anya-e1007n:201": "29.",
  "anya-e1007n:202": " Anārambho hi kammānaṃ,",
  "anya-e1007n:203": "Paṭhamaṃ buddhilakkhaṇaṃ;",
  "anya-e1007n:204": "Niṭṭhaṅgataṃ āraddhassa,",
  "anya-e1007n:205": "Dutiyaṃ buddhilakkhaṇaṃ.",
  "anya-e1007n:206": "Asamekkhitakammantaṃ,",
  "anya-e1007n:207": "Turitābhinipātinaṃ;",
  "anya-e1007n:208": "Sāni kammāni tappenti,",
  "anya-e1007n:209": "Uṇhaṃ vajjhohaṭaṃ mukhe.",
  "anya-e1007n:210": "30.",
  "anya-e1007n:211": " Aphalāni durantāni,",
  "anya-e1007n:212": "Janatā ninditāni ca;",
  "anya-e1007n:213": "Asakyāni ca kammāni,",
  "anya-e1007n:214": "Nārabhetha vicakkhaṇo.",
  "anya-e1007n:215": "31.",
  "anya-e1007n:216": " Ativirodhabhītānaṃ,",
  "anya-e1007n:217": "Saṅkitānaṃ pade pade;",
  "anya-e1007n:218": "Parappavādatāsānaṃ,",
  "anya-e1007n:219": "Dūrato yanti sampadā.",
  "anya-e1007n:220": "Saddamattaṃ na bhetabbaṃ,",
  "anya-e1007n:221": "Loko saddassa gocaro;",
  "anya-e1007n:222": "Yo ca saddaparittāso,",
  "anya-e1007n:223": "Vane bhantamigo hi so.",
  "anya-e1007n:224": "32.",
  "anya-e1007n:225": " Dvinnaṃ taṇḍulathūsānaṃ,",
  "anya-e1007n:226": "Viseso suṭṭhu khāyati;",
  "anya-e1007n:227": "Randhitopi siniddho no,",
  "anya-e1007n:228": "Thuso virasaphāruso;",
  "anya-e1007n:229": "Taṇḍulaṃ siniddhaṃ rasaṃ,",
  "anya-e1007n:230": "Evaṃ lokepi ñāyate.",
  "anya-e1007n:231": "33.",
  "anya-e1007n:232": " Eraṇḍaṃ nissitā valli,",
  "anya-e1007n:233": "Ruhate kiṃ yathābalaṃ;",
  "anya-e1007n:234": "Mahāsālaṃ sunissāya,",
  "anya-e1007n:235": "Ruhate brahataṃ gatā.",
  "anya-e1007n:236": "34.",
  "anya-e1007n:237": " Mettā hi sīmasambhedā,",
  "anya-e1007n:238": "Pakkhapāta vighātikā;",
  "anya-e1007n:239": "Pakkhapātena dukkhanti,",
  "anya-e1007n:240": "Nippakkho vasate sukhaṃ.",
  "anya-e1007n:241": "35.",
  "anya-e1007n:242": " Narā paññā ca laṅkārā,",
  "anya-e1007n:243": "Yathāṭhāne niyujjare;",
  "anya-e1007n:244": "No hi cūḷāmaṇi pāde,",
  "anya-e1007n:245": "Pādukā ca siropari.",
  "anya-e1007n:246": "Ukkuṭṭhe sūra micchanti;",
  "anya-e1007n:247": "Mantīsu akutūhalaṃ;",
  "anya-e1007n:248": "Viyañca annapānamhi;",
  "anya-e1007n:249": "Atthe jāte ca paṇḍitaṃ.",
  "anya-e1007n:250": "36.",
  "anya-e1007n:251": " Pamādo hi tamo loke,",
  "anya-e1007n:252": "Kālo coro bhayānako;",
  "anya-e1007n:253": "Kāyagehaṃ bahuchiddaṃ,",
  "anya-e1007n:254": "Kālacorassa coritaṃ.",
  "anya-e1007n:255": "37.",
  "anya-e1007n:256": " Cañcalo kāladāso hi,",
  "anya-e1007n:257": "Dhītimā kālaissaro;",
  "anya-e1007n:258": "Kālissaro raṭṭhissaraṃ,",
  "anya-e1007n:259": "Ativattati sabbaso.",
  "anya-e1007n:260": "38.",
  "anya-e1007n:261": " Viluppanti 8 dhanaṃ eke,",
  "anya-e1007n:262": "Kālameke anekkhakā;",
  "anya-e1007n:263": "Tesu kālavilopāva,",
  "anya-e1007n:264": "Bhayānakā tikakkhaḷā.",
  "anya-e1007n:265": "39.",
  "anya-e1007n:266": " Raññā raṭṭhahitaṃ kattā,",
  "anya-e1007n:267": "Rañño hitaṃ janehi ve;",
  "anya-e1007n:268": "Desso attahitaṃ dassī,",
  "anya-e1007n:269": "Gārayho kinnu kārako.",
  "anya-e1007n:270": "Attadatthaṃ paratthena;",
  "anya-e1007n:271": "Bahunāpi na hāpaye.",
  "anya-e1007n:272": "40.",
  "anya-e1007n:273": " Yassa upakāro dinno,",
  "anya-e1007n:274": "Upakāraṃ dade puna;",
  "anya-e1007n:275": "Tato pakāraṃ niccheyya,",
  "anya-e1007n:276": "Kataññū dullabho idha.",
  "anya-e1007n:277": "Saccaṃ kireva māhaṃsu,",
  "anya-e1007n:278": "Narā ekacciyā idha;",
  "anya-e1007n:279": "Kaṭṭhaṃ niplavitaṃ seyyo,",
  "anya-e1007n:280": "Natveve kacciyo naro.",
  "anya-e1007n:281": "41.",
  "anya-e1007n:282": " Kāruko sakapaññāya,",
  "anya-e1007n:283": "Mahagghaṃ dārukaṃ kare;",
  "anya-e1007n:284": "Tathā janopi attānaṃ,",
  "anya-e1007n:285": "Mahaggho lokamānito.",
  "anya-e1007n:286": "42.",
  "anya-e1007n:287": " Appaggho hi ayo hemaṃ,",
  "anya-e1007n:288": "Mahagghaṃ chindate yathā;",
  "anya-e1007n:289": "Nigguṇo saguṇaṃ loke,",
  "anya-e1007n:290": "Alakkhīca siriṃ tathā.",
  "anya-e1007n:291": "Sūrassatīnīti",
  "anya-e1007n:292": "Dutiyo bhāgo",
  "anya-e1007n:293": "1.",
  "anya-e1007n:294": " Dhanassa dubbidhaṃ kiccaṃ,",
  "anya-e1007n:295": "Pāpeti uṇṇataṃ dhaniṃ;",
  "anya-e1007n:296": "Adhaniṃ oṇataṃ loke,",
  "anya-e1007n:297": "Sañcine tena taṃ dhanaṃ.",
  "anya-e1007n:298": "2.",
  "anya-e1007n:299": " Vaṭṭate satataṃ sīghaṃ,",
  "anya-e1007n:300": "Kālacakkaṃ avāritaṃ;",
  "anya-e1007n:301": "Tena ghaṭī dinaṃ māso,",
  "anya-e1007n:302": "Vasso bhavatya cīrato.",
  "anya-e1007n:303": "3.",
  "anya-e1007n:304": " Sattunā na hi sandheyya,",
  "anya-e1007n:305": "Ekadā so bhayaṃ karo;",
  "anya-e1007n:306": "Sutattamapi pānīyaṃ,",
  "anya-e1007n:307": "Samayate nu pāvakaṃ.",
  "anya-e1007n:308": "4.",
  "anya-e1007n:309": " Vijahaṃ pakatiṃ yo hi,",
  "anya-e1007n:310": "Vikatiṃ puna gacchati;",
  "anya-e1007n:311": "Sabhāvena ākārena,",
  "anya-e1007n:312": "Vippallāsaṃ sa gacchati;",
  "anya-e1007n:313": "Saṃsumāra gatā godhā,",
  "anya-e1007n:314": "Yathā thī pumavesikā.",
  "anya-e1007n:315": "5.",
  "anya-e1007n:316": " Pakkhaṃ laddhāna uḍḍenti,",
  "anya-e1007n:317": "Upacikā hi vammikā;",
  "anya-e1007n:318": "Nikkhantā maraṇaṃ yanti,",
  "anya-e1007n:319": "Uppatā nippataṃ gatā.",
  "anya-e1007n:320": "6.",
  "anya-e1007n:321": " Sabbaṃpiyassa dajjeyya,",
  "anya-e1007n:322": "Nissesaṃ piyamānasaṃ;",
  "anya-e1007n:323": "Saddhācittaṃ tu no viññū,",
  "anya-e1007n:324": "Saddhāyiko pakkhalito.",
  "anya-e1007n:325": "7.",
  "anya-e1007n:326": " Sakkoti laṅghituṃ byāmaṃ,",
  "anya-e1007n:327": "Mahussāhena yo hi so;",
  "anya-e1007n:328": "Tadaḍḍhaṃ anussāhena,",
  "anya-e1007n:329": "Nossāho tesu thomito;",
  "anya-e1007n:330": "Mahussāho dukkho loke,",
  "anya-e1007n:331": "Anussāho sadā sukho.",
  "anya-e1007n:332": "8.",
  "anya-e1007n:333": " Dukkhamaṃ akkhamanto yo,",
  "anya-e1007n:334": "Piṭṭhikārīca dukkaraṃ,",
  "anya-e1007n:335": "Kadā labheyya so loke,",
  "anya-e1007n:336": "Sukhamaṃ sukaraṃ mudhā;",
  "anya-e1007n:337": "Paccakkhañhi so kareyya,",
  "anya-e1007n:338": "Dukkhamañcāpi dukkaraṃ.",
  "anya-e1007n:339": "9.",
  "anya-e1007n:340": " Anaggho manusso loke,",
  "anya-e1007n:341": "Tosanāposanādinā;",
  "anya-e1007n:342": "Tena so mahagghaṃ kammaṃ,",
  "anya-e1007n:343": "Kare lokahitāyutaṃ.",
  "anya-e1007n:344": "10.",
  "anya-e1007n:345": " Vātena nappabhijjanti,",
  "anya-e1007n:346": "Ninnā veḷū kasā naḷā;",
  "anya-e1007n:347": "Yathāvātaṃ nagacchanti,",
  "anya-e1007n:348": "Tathā care jane kadā.",
  "anya-e1007n:349": "11.",
  "anya-e1007n:350": " Purato ca pacchato ce,",
  "anya-e1007n:351": "Nissayo natthi passato;",
  "anya-e1007n:352": "Adhikaṃ vīriyaṃ hoti,",
  "anya-e1007n:353": "Attanātho tadā bhave.",
  "anya-e1007n:354": "12.",
  "anya-e1007n:355": " Khaṇaṃ ākhubilaṃ sīho,",
  "anya-e1007n:356": "Pāsāṇasakalākulaṃ;",
  "anya-e1007n:357": "Pappoti nakhabhaṅgaṃvā,",
  "anya-e1007n:358": "Phalaṃvā mūsiko bhave.",
  "anya-e1007n:359": "13.",
  "anya-e1007n:360": " Maggamuḷhā janā andhā,",
  "anya-e1007n:361": "Amaggā maggasaññino;",
  "anya-e1007n:362": "Tatheva duppaññā muḷhā,",
  "anya-e1007n:363": "Tathatthaṃ nāva bujjhare.",
  "anya-e1007n:364": "14.",
  "anya-e1007n:365": " Piyarūpaṃ vīrarūpaṃ,",
  "anya-e1007n:366": "Duvidhā rūpasampadā,",
  "anya-e1007n:367": "Nāriṃ icche piyarūpiṃ,",
  "anya-e1007n:368": "Purisaṃ vīrarūpakaṃ.",
  "anya-e1007n:369": "15.",
  "anya-e1007n:370": " Pajjalanti hi khajjotā,",
  "anya-e1007n:371": "Pakkhacālanakammunā;",
  "anya-e1007n:372": "Kusitā supitā nete,",
  "anya-e1007n:373": "Tathā janāpi kammikā.",
  "anya-e1007n:374": "16.",
  "anya-e1007n:375": " Kummasaṅkocamopamya,",
  "anya-e1007n:376": "Niggahamapi saṃkhame;",
  "anya-e1007n:377": "Pattakāle tu nītiñño,",
  "anya-e1007n:378": "Kaṇhasappova uṭṭhahe.",
  "anya-e1007n:379": "17.",
  "anya-e1007n:380": " Bhakkhasesaṃ nakhādanti,",
  "anya-e1007n:381": "Sīhā unnatacetasā;",
  "anya-e1007n:382": "Paraṃpi napaṇāmenti,",
  "anya-e1007n:383": "Vuddhikāmā tathā care.",
  "anya-e1007n:384": "18.",
  "anya-e1007n:385": " Na va vasso samuppanno,",
  "anya-e1007n:386": "Khīṇo purāṇahāyano;",
  "anya-e1007n:387": "Navavasse navā mettā,",
  "anya-e1007n:388": "Bhāvitabbā hitesinā.",
  "anya-e1007n:389": "19.",
  "anya-e1007n:390": " Santāpayanti kamayāpyabhujaṃ na rogā,",
  "anya-e1007n:391": "Dummantinaṃ kamupayanti na nītidosā;",
  "anya-e1007n:392": "Kaṃ srī na mānayati kaṃ na ca hanti maccu,",
  "anya-e1007n:393": "Kaṃ thīkatā na visayā paripīḷayanti.",
  "anya-e1007n:394": "(Vasantatilakāgāthā.)",
  "anya-e1007n:395": "20.",
  "anya-e1007n:396": " (Ka) byāmamattena daṇḍena,",
  "anya-e1007n:397": "Yolumbya udakaṃ mine;",
  "anya-e1007n:398": "Agambhīraṃ gambhīraṃvā,",
  "anya-e1007n:399": "Agādhe maññi gambhīraṃ.",
  "anya-e1007n:400": "(Kha)",
  "anya-e1007n:401": "Tathā mando saññāṇena,",
  "anya-e1007n:402": "Agādhe maññi paṇḍitaṃ;",
  "anya-e1007n:403": "Samāsamaṃ na jānāti,",
  "anya-e1007n:404": "Bahvappaṃ tikkhamandataṃ.",
  "anya-e1007n:405": "21.",
  "anya-e1007n:406": " Duṭṭhakamme saṅgamanti,",
  "anya-e1007n:407": "Chekakamme ca no idha;",
  "anya-e1007n:408": "Maccuṃ vahanti sīsena,",
  "anya-e1007n:409": "Te muḷhā muḷhasaṅgamā.",
  "anya-e1007n:410": "22.",
  "anya-e1007n:411": " Duvidho saṅgamo loke,",
  "anya-e1007n:412": "Ujuko kuṭilo bhave;",
  "anya-e1007n:413": "Ujukova pasaṃseyyo,",
  "anya-e1007n:414": "Nohyañño sājasaṅgamo.",
  "anya-e1007n:415": "Te iminā upāyena samaggā sammodamānā mahā bhittipiṭṭhikāya vasanti. (Mahosadhajātaka aṭṭhakathā)",
  "anya-e1007n:416": "23.",
  "anya-e1007n:417": " Yūthikā pupphate nohi,",
  "anya-e1007n:418": "Siñcitāpi punappunaṃ;",
  "anya-e1007n:419": "Pupphate sampatte kāle,",
  "anya-e1007n:420": "Evaṃ dhāretha vīriyaṃ.",
  "anya-e1007n:421": "24.",
  "anya-e1007n:422": " Dhanuccayo dhanakkhepo,",
  "anya-e1007n:423": "Duvidhā hi dhanākati;",
  "anya-e1007n:424": "Dhanuccaye nayo atthi,",
  "anya-e1007n:425": "Dhanakkhepamhi no idha.",
  "anya-e1007n:426": "25.",
  "anya-e1007n:427": " Amātā pitarasaṃ vaḍḍhaṃ,",
  "anya-e1007n:428": "Jūtakārañca cañcalaṃ;",
  "anya-e1007n:429": "Nālapeyya visesaññū,",
  "anya-e1007n:430": "Yadicche siddhi mattano.",
  "anya-e1007n:431": "Haliddirāgaṃ kapicittaṃ,",
  "anya-e1007n:432": "Purisaṃ rāgavirāginaṃ;",
  "anya-e1007n:433": "Edisaṃ tāta māsevi,",
  "anya-e1007n:434": "Nimmanussaṃpi ce siyā.",
  "anya-e1007n:435": "26.",
  "anya-e1007n:436": " Guṇā guṇaññūsu guṇā bhavanti,",
  "anya-e1007n:437": "Te nigguṇaṃ patvā bhavanti dosā;",
  "anya-e1007n:438": "Āsādyatoyā pabhavanti najjo,",
  "anya-e1007n:439": "Samudramāsajja bhavantyapeyyā.",
  "anya-e1007n:440": "(Upajātigāthā)",
  "anya-e1007n:441": "27.",
  "anya-e1007n:442": " Silārūpaṃ nimminanti,",
  "anya-e1007n:443": "Koṭṭetvāna punappunaṃ;",
  "anya-e1007n:444": "Koṭṭakova tathā bālā,",
  "anya-e1007n:445": "Sādhuṃ ovajja nimmitā.",
  "anya-e1007n:446": "Cāṇakyanītilā gāthā",
  "anya-e1007n:447": "Lālane bahavo dosā,",
  "anya-e1007n:448": "Tāḷane bahavo guṇā;",
  "anya-e1007n:449": "Tasmā puttañca sissañca,",
  "anya-e1007n:450": "Tāḷaye na ca lālaye.",
  "anya-e1007n:451": "28.",
  "anya-e1007n:452": " Atītassa hi mittassa,",
  "anya-e1007n:453": "Yo ce dosaṃ pakāsaye;",
  "anya-e1007n:454": "So have paccuppannassa,",
  "anya-e1007n:455": "Dosaṃ bhāseti ñāyati.",
  "anya-e1007n:456": "29.",
  "anya-e1007n:457": " Latāviya sevakā te,",
  "anya-e1007n:458": "Ye nissayaṃ palambare;",
  "anya-e1007n:459": "Nissayassa vināsena,",
  "anya-e1007n:460": "Bhūmyaṃ senti anāthakā.",
  "anya-e1007n:461": "30.",
  "anya-e1007n:462": " Dosasiṅgehi vijjhanto,",
  "anya-e1007n:463": "Mānakhūrehi akkamaṃ;",
  "anya-e1007n:464": "Bhayaṃ karoti lokamhi,",
  "anya-e1007n:465": "Gova bālo vihiṃsako.",
  "anya-e1007n:466": "31.",
  "anya-e1007n:467": " Ādo upari lokoyaṃ,",
  "anya-e1007n:468": "Ujulekhāya tiṭṭhati;",
  "anya-e1007n:469": "Musāvātehi taṃlokaṃ,",
  "anya-e1007n:470": "Nipātesi anajjavaṃ.",
  "anya-e1007n:471": "32.",
  "anya-e1007n:472": " Sughaṭaṃ kumbhakārena,",
  "anya-e1007n:473": "Nāraho paribhuñjituṃ;",
  "anya-e1007n:474": "Tathūpamāya vekkheyya,",
  "anya-e1007n:475": "Sakammaparakammani.",
  "anya-e1007n:476": "33.",
  "anya-e1007n:477": " Anantaraṃsī sūropi,",
  "anya-e1007n:478": "Nasakkoti ghanaṃ tamaṃ;",
  "anya-e1007n:479": "Vijjhituṃ raṃsiyā loke,",
  "anya-e1007n:480": "Tathā madanamohitā;",
  "anya-e1007n:481": "Nasakkonti madaṃ bhetvā,",
  "anya-e1007n:482": "Paññābhāya pabhāsituṃ.",
  "anya-e1007n:483": "34.",
  "anya-e1007n:484": " Khedaveraṃ daliddamhi,",
  "anya-e1007n:485": "Bhogimhi rogupaddavaṃ;",
  "anya-e1007n:486": "Dessaverañca āṇimhi,",
  "anya-e1007n:487": "Passe lokassa veritaṃ.",
  "anya-e1007n:488": "35.",
  "anya-e1007n:489": " Saṃladdhena subhogena,",
  "anya-e1007n:490": "Jīvaṃ suddhaṃ kare nijaṃ;",
  "anya-e1007n:491": "Seṭṭho so tena jīvena,",
  "anya-e1007n:492": "Jeguccho malajīviko.",
  "anya-e1007n:493": "36.",
  "anya-e1007n:494": " Vajira puppharāgānaṃ,",
  "anya-e1007n:495": "Visesaṃ yo nabujjhati;",
  "anya-e1007n:496": "Kathañhi so vikkīṇeyya,",
  "anya-e1007n:497": "Kīṇeyya vā yathātathaṃ.",
  "anya-e1007n:498": "37.",
  "anya-e1007n:499": " Kippīli kopi cintetvā,",
  "anya-e1007n:500": "Pabbataṃ bhettu mussahaṃ;",
  "anya-e1007n:501": "Abalā tanumajjhattā,",
  "anya-e1007n:502": "Cintā hasyāva sā mudhā.",
  "anya-e1007n:503": "38.",
  "anya-e1007n:504": " Jātamattaṃ na yo sattuṃ,",
  "anya-e1007n:505": "Rogañcūpasamaṃ naye;",
  "anya-e1007n:506": "Mahābalopi teneva,",
  "anya-e1007n:507": "Vuddhiṃpatvā sa haññate.",
  "anya-e1007n:508": "39.",
  "anya-e1007n:509": " Sajīvamaṃsabhakkhehi,",
  "anya-e1007n:510": "Sadāṭhīhi mukhehi bho;",
  "anya-e1007n:511": "Biḷārabyagghasīhānaṃ,",
  "anya-e1007n:512": "Nihīnāni anekadhā;",
  "anya-e1007n:513": "Tikkhāni kharavādāni,",
  "anya-e1007n:514": "Manussānaṃ mukhāni ve.",
  "anya-e1007n:515": "40.",
  "anya-e1007n:516": " Viluppantā vidhāvanti,",
  "anya-e1007n:517": "Sajīvavuttikammunā;",
  "anya-e1007n:518": "Janā tena vihaññanti,",
  "anya-e1007n:519": "Caranti dhammavemukhā.",
  "anya-e1007n:520": "41.",
  "anya-e1007n:521": " Sulabhaṃ lokiyaṃ loke,",
  "anya-e1007n:522": "Sāsanīyaṃva dullabhaṃ;",
  "anya-e1007n:523": "Dullabhaṃ taṃ vamaññanto,",
  "anya-e1007n:524": "Eso bālatamo bhave.",
  "anya-e1007n:525": "42.",
  "anya-e1007n:526": " Yo patittha agyāvāṭaṃ,",
  "anya-e1007n:527": "Mohā taṃ upakārituṃ;",
  "anya-e1007n:528": "Aññorohi tadā vāṭaṃ,",
  "anya-e1007n:529": "Dutīyo muḷhamuḷhako.",
  "anya-e1007n:530": "43.",
  "anya-e1007n:531": " Byaggho āvudhaviddho hi,",
  "anya-e1007n:532": "Akā duṭṭhāni ninnadaṃ;",
  "anya-e1007n:533": "Tatheva sādhusatthena,",
  "anya-e1007n:534": "Viddho bālo pakuppito.",
  "anya-e1007n:535": "44.",
  "anya-e1007n:536": " Pivanti lohitaṃ ḍaṃsā,",
  "anya-e1007n:537": "Anto tuṇḍena makkhikā;",
  "anya-e1007n:538": "Bahiddhā parivārenti,",
  "anya-e1007n:539": "Jano tena ḍaṃsāyaye.",
  "anya-e1007n:540": "45.",
  "anya-e1007n:541": " Adhanassa khaṇo appo,",
  "anya-e1007n:542": "Saddhammo appakālino;",
  "anya-e1007n:543": "Appako tena yuñjeyyuṃ,",
  "anya-e1007n:544": "Khaṇaṃ bahuṃ labhetave."
}