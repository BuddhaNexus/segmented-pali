{
  "anya-e0607n:6": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e0607n:7": "Pajjamadhu",
  "anya-e0607n:8": "Ānanda rañña ratanādi mahā yatinda,",
  "anya-e0607n:9": "Niccappabuddha padumappiya sevinaṅgī;",
  "anya-e0607n:10": "Buddhappiyena ghana buddha guṇappiyena,",
  "anya-e0607n:11": "Therālinā racita pajjamadhuṃ pibantu.",
  "anya-e0607n:12": "1.",
  "anya-e0607n:13": "Uṇṇāpapuṇṇasasimaṇdalato galitvā,",
  "anya-e0607n:14": "Pādambujaṅguli dalaṭṭha sudhā lavānaṃ;",
  "anya-e0607n:15": "Pantīva satthu nakhapanti pajāvisesaṃ,",
  "anya-e0607n:16": "Pīṇetu suddha sukhitammaṇa tunḍapītā.",
  "anya-e0607n:17": "2.",
  "anya-e0607n:18": "Khittāya māraripunā parivatya satthu,",
  "anya-e0607n:19": "Pādassayā jita disāya sitattalāya;",
  "anya-e0607n:20": "Yā jeti kañcana sarāvaliyā siriṃ sā,",
  "anya-e0607n:21": "Detanginaṃ raṇajayaṅgulipantikantā.",
  "anya-e0607n:22": "3.",
  "anya-e0607n:23": "Sovaṇṇa vaṇṇa sukhumacchavi somma kumma,",
  "anya-e0607n:24": "Piṭṭhīva piṭṭhi kamatunnati bhāti yesaṃ;",
  "anya-e0607n:25": "Tesuppatiṭṭhitasukomaladīghapaṇhi,",
  "anya-e0607n:26": "Pādā jinassa padadantu padaṃ janassa.",
  "anya-e0607n:27": "4.",
  "anya-e0607n:28": "Acchera paṅkajasiraṃ siriyā sakāya,",
  "anya-e0607n:29": "Ye maddino viya caranti saroja sīse;",
  "anya-e0607n:30": "Sañcumbitā viya ca tāni parāga rāgā,",
  "anya-e0607n:31": "Te nīrajā munipadā padadantu lakkhiṃ.",
  "anya-e0607n:32": "5.",
  "anya-e0607n:33": "Agāmi kāla jana maṅgala bhattu bhāvaṃ,",
  "anya-e0607n:34": "Vyākattumatra kusaleniva nimmitāni;",
  "anya-e0607n:35": "Yātrāsumaṭṭhasatamaṅgalakkhaṇāni,",
  "anya-e0607n:36": "Sādhetu naṃ padayugaṃ jayamaṅgalāni.",
  "anya-e0607n:37": "6.",
  "anya-e0607n:38": "Sassevijantuvarasantipurappavese,",
  "anya-e0607n:39": "Niccaṃ susajja ṭhapitāniva maṅgalāya;",
  "anya-e0607n:40": "Ye te dadhanti kalamaṅgalalakkhaṇāni,",
  "anya-e0607n:41": "Vattantu te jinapadā jayamaṅgalāya.",
  "anya-e0607n:42": "7.",
  "anya-e0607n:43": "Sabbebhibhūya sapadesu nipātanassa,",
  "anya-e0607n:44": "Saññāṇakaṃ viya yadassitasabbaloko;",
  "anya-e0607n:45": "Pādātyadhokatatilokasirovarā pi,",
  "anya-e0607n:46": "Lokaṃ puṇantu jayamaṅgalakāraṇāni.",
  "anya-e0607n:47": "8.",
  "anya-e0607n:48": "Lokattayekasaraṇattavibhāvanāya,",
  "anya-e0607n:49": "Sajjo va tiṭṭhati yahiṃ suvibhattaloko;",
  "anya-e0607n:50": "Taṃ sabbalokapaṭibimbitadappaṇābhaṃ,",
  "anya-e0607n:51": "Pādadvayaṃ janasusajjanahetu hotu.",
  "anya-e0607n:52": "9.",
  "anya-e0607n:53": "Lokuttarāya siriyādhigamāya suṭṭhu,",
  "anya-e0607n:54": "Rajanti yattha diguṇāniva pātu bhūtā;",
  "anya-e0607n:55": "Cakkāsanābhisahanemisahassarāni,",
  "anya-e0607n:56": "Tyaṅghī disantu sakalissariyaṃ janassa.",
  "anya-e0607n:57": "10.",
  "anya-e0607n:58": "Yatrullasanti duvidhāniva pātū bhūtā,",
  "anya-e0607n:59": "Dhammassasabbabhuvanassa ca issaratte;",
  "anya-e0607n:60": "Cakkāni cakkasadisāni sudassanassa,",
  "anya-e0607n:61": "Tānajja jantu saraṇā caraṇāni hontu.",
  "anya-e0607n:62": "11.",
  "anya-e0607n:63": "Sattesu vacchatu sirī sirivacchakena,",
  "anya-e0607n:64": "Sovatthi sotthimanutiṭṭhatu puggalesu;",
  "anya-e0607n:65": "Nandiṃ janānamanuvattatu nandivattī,",
  "anya-e0607n:66": "Sīsānalaṃkurutu pādavataṃsako pi.",
  "anya-e0607n:67": "12.",
  "anya-e0607n:68": "Bhaddāya pīṭhamupagacchatu bhaddapīṭhaṃ,",
  "anya-e0607n:69": "Vuddhiṃ janānamanuvattatu vaddhamānaṃ;",
  "anya-e0607n:70": "Puṇṇattamaṅgimanukubbatu puṇṇakumbho,",
  "anya-e0607n:71": "Pāti ca pātu satataṃ janataṃ apāyā.",
  "anya-e0607n:72": "13.",
  "anya-e0607n:73": "Setātapattamapanetamaghātape taṃ,",
  "anya-e0607n:74": "Khaggo vichindatu sadā duritārivagge;",
  "anya-e0607n:75": "Saṃklesadāhamapanetu satālavaṇṭa,",
  "anya-e0607n:76": "Saṃvījanī kumatimakkhikamorahattho.",
  "anya-e0607n:77": "14.",
  "anya-e0607n:78": "Ākaḍḍhano janavilocanamattaninnaṃ,",
  "anya-e0607n:79": "Vāretu sabbagativāranamaṅkuso so;",
  "anya-e0607n:80": "Pādambujassirivilāsaniketanaṃ va,",
  "anya-e0607n:81": "Pāsādalakhaṇamupetu manopasādaṃ.",
  "anya-e0607n:82": "15.",
  "anya-e0607n:83": "Pāṇīnamattabhajataṃ varapuṇṇapattaṃ,",
  "anya-e0607n:84": "Sammā dadātu padanissitapuṇṇapatto;",
  "anya-e0607n:85": "Pādesu jantu manabandhanadāmabhūtaṃ,",
  "anya-e0607n:86": "Dāmaṃ dametu vimalaṃ janatammanāni.",
  "anya-e0607n:87": "16.",
  "anya-e0607n:88": "Uṇhīsakuppalamaṇīpadumehi pādā,",
  "anya-e0607n:89": "Sassevijantukaraṇāni vibhūsayantu;",
  "anya-e0607n:90": "Sannettanāvupagatānamanagghakāni,",
  "anya-e0607n:91": "Bojjhaṅgasattaratanāni dade samuddo.",
  "anya-e0607n:92": "17.",
  "anya-e0607n:93": "Uttuṅga niccalaguṅā jitatāya niccaṃ,",
  "anya-e0607n:94": "Sevīva pādasiri nicca samubbahaṃ va;",
  "anya-e0607n:95": "Atrāpi sakkabhavanubbahaṇe niyutto,",
  "anya-e0607n:96": "Pādaṭṭhameru bhavataṃ bhavataṃ vibhūtyā.",
  "anya-e0607n:97": "18.",
  "anya-e0607n:98": "So cakkavāḷasikharī pyavataṃ samantā,",
  "anya-e0607n:99": "Sabbūpasaggavisarājanataṃ samaggaṃ;",
  "anya-e0607n:100": "Dīpā puthūpi caturo dvisahassa khuddā,",
  "anya-e0607n:101": "Dhārentvapāyapatamānamadatva jantuṃ.",
  "anya-e0607n:102": "19.",
  "anya-e0607n:103": "Sūro pabodhayatu jantu saroruhāni,",
  "anya-e0607n:104": "Cando pasāda kumudāni manodahesu;",
  "anya-e0607n:105": "Nakkhattajātamakhilaṃ subhatāya hotu,",
  "anya-e0607n:106": "Cakkaṃ dhajaṃ ripujayāya jayaddhajāya.",
  "anya-e0607n:107": "20.",
  "anya-e0607n:108": "Jetuṃ sasaṃsada-sudassana-cakkavatti,",
  "anya-e0607n:109": "Cakkānugantalalitaṃ yahimāvaheyya;",
  "anya-e0607n:110": "Cakkāṇuvatti parisāvata-cakkavatti,",
  "anya-e0607n:111": "Naṃvattataṃ padayugaṃ janatā hitāya.",
  "anya-e0607n:112": "21.",
  "anya-e0607n:113": "Pujetumāgata vatā vajirāsanaṭṭha,",
  "anya-e0607n:114": "Mindena chaḍḍita mahāvijayuttarākhyaṃ;",
  "anya-e0607n:115": "Saṃkhaṃ paviṭṭhamiva mārabhayā padādho,",
  "anya-e0607n:116": "Pādaṭṭhasaṃkhamiha vattatu santiyā vo.",
  "anya-e0607n:117": "22.",
  "anya-e0607n:118": "Sovaṇṇamacchayugalaṃ sivabhatta bhoge,",
  "anya-e0607n:119": "Icchā bahūpakaraṇaṃ bhavataṃ janānaṃ;",
  "anya-e0607n:120": "Kumbhīladhiggahitato va padutthacittā,",
  "anya-e0607n:121": "Pādambujākara vigāhi tu nopahontu.",
  "anya-e0607n:122": "23.",
  "anya-e0607n:123": "Sattāpagā janamanoja male jahantu,",
  "anya-e0607n:124": "Saṃklesadāhamapanentu dahā ca satta;",
  "anya-e0607n:125": "Selā ca satta vidadhantu janassa tānaṃ,",
  "anya-e0607n:126": "Lokappasiddhijanane bhavataṃ patākā.",
  "anya-e0607n:127": "24.",
  "anya-e0607n:128": "Pāṭaṅki santi gamane bhavatūpakārā,",
  "anya-e0607n:129": "Dāhettanesu jahataṃ padacāmaraṃ taṃ;",
  "anya-e0607n:130": "Sallokalocanamahussava-ussitaṃ va,",
  "anya-e0607n:131": "Vatteyya toraṇamanuttaramaṅgalāya.",
  "anya-e0607n:132": "25.",
  "anya-e0607n:133": "Yasmiṃ miginda gata bhīti balāva daḍḍha,",
  "anya-e0607n:134": "Dānā natā siravidāraṇa pīḷitāva;",
  "anya-e0607n:135": "Nālāgirī karivaro girimekhalo ca,",
  "anya-e0607n:136": "Taṃ sīhavikkamapadaṃ hanatā ghadantiṃ.",
  "anya-e0607n:137": "26.",
  "anya-e0607n:138": "Pāpāhino hanatu pādasuvaṇṇarājā,",
  "anya-e0607n:139": "Vyagghādhipo kalijane adataṃ asesaṃ;",
  "anya-e0607n:140": "Vālāha-assapati sampatituṃ adatvā,",
  "anya-e0607n:141": "Pāyesu pāpayatu santipurampajāyo.",
  "anya-e0607n:142": "27.",
  "anya-e0607n:143": "Chaddanta danti lalitaṃ galitaṃ rusamhā,",
  "anya-e0607n:144": "Luddetta dubbhini dise acalaṃ dadhāno;",
  "anya-e0607n:145": "Pādaṭṭhahatthipati sampati jantutāse,",
  "anya-e0607n:146": "Tāsetu hāsamaparandisataṃ satānaṃ.",
  "anya-e0607n:147": "28.",
  "anya-e0607n:148": "Sabbaṅgino caraṇuposatha hatthirājā,",
  "anya-e0607n:149": "Pāpetu sabbacatudīpikarajjalakkhiṃ;",
  "anya-e0607n:150": "Kittīva pādaparicārikatā niyuttā,",
  "anya-e0607n:151": "Kelasaselapaṭimā hitamācareyya.",
  "anya-e0607n:152": "29.",
  "anya-e0607n:153": "Sāmissa haṃsasamaye dahapāsabaddha,",
  "anya-e0607n:154": "Māsīna vesagamako viya pādahaṃso;",
  "anya-e0607n:155": "Nigghosa gantijitato viya mūgapakkho,",
  "anya-e0607n:156": "Yāretu sabba janatā bhavagantukattaṃ.",
  "anya-e0607n:157": "30.",
  "anya-e0607n:158": "Ohāya dibbasarasiṃ khilaloka sabba,",
  "anya-e0607n:159": "Rammaṅghivāpimavagāhitavāva pāde;",
  "anya-e0607n:160": "Erāvaṇo karivaro manasābhiruḷhe,",
  "anya-e0607n:161": "Jantuṃ purindadapuraṃ nayataṃ va sīghaṃ.",
  "anya-e0607n:162": "31.",
  "anya-e0607n:163": "Hitvā sakambhavanamaṅghinisevanattha,",
  "anya-e0607n:164": "Māgamma ramma taratāyiha nissito va;",
  "anya-e0607n:165": "Pāletva mūni padavāpitaraṅgabhaṅgi,",
  "anya-e0607n:166": "Mangī karontatanuvāsuki nāgarājā.",
  "anya-e0607n:167": "32.",
  "anya-e0607n:168": "Nāthassa kañcanasikhāvalajātilīla,",
  "anya-e0607n:169": "Māvikaraṃ va padanissitamorarājā;",
  "anya-e0607n:170": "Taṃ dhammadesanaraveniva luddakassa,",
  "anya-e0607n:171": "Lokassa pāpaphaṇino hanataṃ asesaṃ.",
  "anya-e0607n:172": "33.",
  "anya-e0607n:173": "Saṃsārasāgaragate sadhane jane te,",
  "anya-e0607n:174": "Netampade kalacatummukhahemanāvā;",
  "anya-e0607n:175": "Nibbāṇapaṭṭanavaraṃ bharukacchakantaṃ,",
  "anya-e0607n:176": "Suppārapaṇḍita gatā viya āsunāvā.",
  "anya-e0607n:177": "34.",
  "anya-e0607n:178": "Sambodhi ñāṇa paripācayato munissa,",
  "anya-e0607n:179": "Bhatto yathā himavataddi samādhihetu;",
  "anya-e0607n:180": "Evammanena bhajataṃ himavaddipāde,",
  "anya-e0607n:181": "Sambodhiñāṇa paripācanahetu hotu.",
  "anya-e0607n:182": "35.",
  "anya-e0607n:183": "Daḷhaṃ parājitatayā muninā sarena,",
  "anya-e0607n:184": "Suññassaropagata pañjara bandhanova;",
  "anya-e0607n:185": "So pādapañjaragato karavīkapakkhī,",
  "anya-e0607n:186": "Sabbesamappīyāvacañjahatā bhavantaṃ.",
  "anya-e0607n:187": "36.",
  "anya-e0607n:188": "Te cakkavāka makarā api koñca jīvaṃ,",
  "anya-e0607n:189": "Jīvādi pakkhivisarā sarasīva bhuttaṃ;",
  "anya-e0607n:190": "Vessantarena caraṇambuji nibbhajantā,",
  "anya-e0607n:191": "Jantu tahiṃ viya pade suramentu niccaṃ.",
  "anya-e0607n:192": "37.",
  "anya-e0607n:193": "Taṃ candakinnaragatiṃva gatassa bodhi,",
  "anya-e0607n:194": "Sattassa tassa sapajāpatikassa bhāvaṃ;",
  "anya-e0607n:195": "Saṃsūcayanta pada kinnara kinnarī ve,",
  "anya-e0607n:196": "Sāmaggimagga paṭi pattisu pāpayantu.",
  "anya-e0607n:197": "38.",
  "anya-e0607n:198": "Saṃrājadhānimusabho vahatagga bhāraṃ,",
  "anya-e0607n:199": "Pītippayo pajanayeyya savacchadhenu;",
  "anya-e0607n:200": "Sassevino abhiramentu chakāmasaggā,",
  "anya-e0607n:201": "Dhārentu jhāyimiha soḷasa dhātudhāmā.",
  "anya-e0607n:202": "39.",
  "anya-e0607n:203": "Sutvā jinassa karavīka sarammanuññaṃ,",
  "anya-e0607n:204": "Aññoñña bhītirahitā api paccanīkā;",
  "anya-e0607n:205": "Hitvā gatiṃ viya ṭhitā padasattarūpā,",
  "anya-e0607n:206": "Sabbaṃ bhavassita janānagatiṃ hanantu.",
  "anya-e0607n:207": "40.",
  "anya-e0607n:208": "Sovaṇṇa kāhaḷa yugo pamamindirāya,",
  "anya-e0607n:209": "Sannīrapuppha mukulopamamussavāya;",
  "anya-e0607n:210": "Niccaṃ susajja ṭhapitaṃ muni tiṭṭhatante,",
  "anya-e0607n:211": "Jaṅghādvayaṃ janavilocana maṅgalāya.",
  "anya-e0607n:212": "41.",
  "anya-e0607n:213": "Lakhyā vilāsa mukuradvaya sannikāsaṃ,",
  "anya-e0607n:214": "Tāḍaṅka maṇḍana viḍambakamaṃsu saṇḍaṃ;",
  "anya-e0607n:215": "Jānudvayaṃ laḷita sāgara bubbalābhaṃ,",
  "anya-e0607n:216": "Hotaṃ jagattaya nijatta vibhūsitunte.",
  "anya-e0607n:217": "42.",
  "anya-e0607n:218": "Chaddanti dinna varadanta yugopamānā,",
  "anya-e0607n:219": "Taṃ hatthi soṇḍa kama puṇṇa guṇā tavorū;",
  "anya-e0607n:220": "Līla payodhi siri keḷi suvaṇṇarambhā,",
  "anya-e0607n:221": "Khandhāva dentu paripuṇṇa gune janānāṃ.",
  "anya-e0607n:222": "43.",
  "anya-e0607n:223": "Jaṅghakkha kadvya samappita cittapāda,",
  "anya-e0607n:224": "Cakkadvayī manamanojahayo mune te;",
  "anya-e0607n:225": "Sonī ratho sirivaho manasā bhiruḷhaṃ,",
  "anya-e0607n:226": "Lokattayaṃ sivapuraṃ lahu pāpayātu.",
  "anya-e0607n:227": "44.",
  "anya-e0607n:228": "Rammora pākaṭa taṭāka taṭā savanta,",
  "anya-e0607n:229": "Romāvalī jala panālika koṭikaṭṭhā;",
  "anya-e0607n:230": "Nābhī gabhīra sarasī siri keḷitā te,",
  "anya-e0607n:231": "Sassevinaṃ vyasana ghammamalaṃ sametu.",
  "anya-e0607n:232": "45.",
  "anya-e0607n:233": "Kanticchaṭā luḷita rūpa payodhi nābhi,",
  "anya-e0607n:234": "Āvaṭṭa vaṭṭita nimujjita sabbaloko;",
  "anya-e0607n:235": "Sobhagga toya nivahaṃ visaso pivitvā,",
  "anya-e0607n:236": "Lokuttarādi sukha mucchitataṃ payātu.",
  "anya-e0607n:237": "46.",
  "anya-e0607n:238": "Gambhīra cittarahadaṃ paripūrayitvā,",
  "anya-e0607n:239": "Taṃ sandamāna karuṇambu pavāha tulyā;",
  "anya-e0607n:240": "Romālivallihari nābhi subhālavālā,",
  "anya-e0607n:241": "Detaṃ lahuṃ sivaphalaṃ bhajataṃ mune te.",
  "anya-e0607n:242": "47.",
  "anya-e0607n:243": "Cārūra sāriphalako kuṭilagga loma,",
  "anya-e0607n:244": "Pantī vibhatti sahito siri keḷi sajjo;",
  "anya-e0607n:245": "Saggāpavagga sukha jūtaka keḷi hetu,",
  "anya-e0607n:246": "Hotaṃ tiloka sukha jūtaka soṇḍakānaṃ.",
  "anya-e0607n:247": "48.",
  "anya-e0607n:248": "Gambhīra citta rahado dara gāhamāna,",
  "anya-e0607n:249": "Mettādayā kari vadhū kara sanni kāsā;",
  "anya-e0607n:250": "Sabbaṅginaṃ sivaphalaṃ tanu deva rukkhe,",
  "anya-e0607n:251": "Sākhā sakhā tava bhujā bhajataṃ dadantu.",
  "anya-e0607n:252": "49.",
  "anya-e0607n:253": "Nihāra bindu sahitaggadalopa sobhi,",
  "anya-e0607n:254": "Byālamba ratta padumadvaya bhaṅgi bhājā;",
  "anya-e0607n:255": "Pāpārisīsalunateniva ratta rattā,",
  "anya-e0607n:256": "Rattā karā tava bhavumbhuvi maṅgalāya.",
  "anya-e0607n:257": "50.",
  "anya-e0607n:258": "Rupassirī carita caṅkama vibbhamā te,",
  "anya-e0607n:259": "Piṭṭhī yathā kalala muddhani setu bhūtā;",
  "anya-e0607n:260": "Evaṃ bhavaṇṇava samuttaraṇāya setu,",
  "anya-e0607n:261": "Hotammahākanaka saṃkama sannikāsā.",
  "anya-e0607n:262": "51.",
  "anya-e0607n:263": "Saddhamma desana manohara bherināda,",
  "anya-e0607n:264": "Saṃcāraṇe sivapuraṃ visituṃ janānaṃ;",
  "anya-e0607n:265": "Gīvā suvaṇṇamaya cāru mutiṅga bheri,",
  "anya-e0607n:266": "Bhāvambhajā bhavatu bhūta vibhūtiyā te.",
  "anya-e0607n:267": "52.",
  "anya-e0607n:268": "Lakhī nivāsa vadanambuja matta ninna,",
  "anya-e0607n:269": "Mākaḍḍhayaṃ jana vilocana cañcarīke;",
  "anya-e0607n:270": "Sorabbha dhamma makaranda nisandamānaṃ,",
  "anya-e0607n:271": "Piṇetu tena sarasena sabhā jane te.",
  "anya-e0607n:272": "53.",
  "anya-e0607n:273": "Lakhī samāruhita vattarathe rathaṅga,",
  "anya-e0607n:274": "Dvandānu kāri miga rāja kapola līlaṃ;",
  "anya-e0607n:275": "Tādaṅka maṇḍalayugaṃ viya kaṇṇabhājaṃ,",
  "anya-e0607n:276": "Gaṇḍatthaladvyamalaṃkurutaṃ janatte.",
  "anya-e0607n:277": "54.",
  "anya-e0607n:278": "Lāvaṇṇa maṇṇava pavāḷa latā dvayābhaṃ,",
  "anya-e0607n:279": "Tandeha deva taru pallava kante mantaṃ;",
  "anya-e0607n:280": "Vattāravinda makaranda parājisobhaṃ,",
  "anya-e0607n:281": "Rattādharadvayamadho kurutaṃ janāghaṃ.",
  "anya-e0607n:282": "55.",
  "anya-e0607n:283": "Uṇṇā sakuntigata matthaka natthu kūpa,",
  "anya-e0607n:284": "Subbhū lakāra sahitoṭṭha pavāḷa nāvā;",
  "anya-e0607n:285": "Gattuttararaṇṇava gatā tava jantukānaṃ,",
  "anya-e0607n:286": "Hotaṃ bhavaṇṇava samuttaranaya nātha.",
  "anya-e0607n:287": "56.",
  "anya-e0607n:288": "Isaṃ vikāsa padumodara kesarāli,",
  "anya-e0607n:289": "Līlā vinaddha rucirā tava danta panti;",
  "anya-e0607n:290": "Vānī vadhū dharita mālati mālya tulyā,",
  "anya-e0607n:291": "Tassaṃ jānassa manarañjana mācareyya.",
  "anya-e0607n:292": "57.",
  "anya-e0607n:293": "Saddhamma nijjhara suratta silātalābhā,",
  "anya-e0607n:294": "Jivhā vacī naṭa vadhū kala raṅga bhūtā;",
  "anya-e0607n:295": "Saddhamma seṭṭha taraṇī nihitappiyā te,",
  "anya-e0607n:296": "Saṃsāra sāgara samuttaraṇāya hotu.",
  "anya-e0607n:297": "58.",
  "anya-e0607n:298": "Dantaṃsu kañcukīta rattadharo padhāne,",
  "anya-e0607n:299": "Jivhā suratta sayane mukha mandiraṭṭhe;",
  "anya-e0607n:300": "Āmokkha mutti vadhuyā sayitāya tuyhaṃ,",
  "anya-e0607n:301": "Kubbantu saṃgama malaṃ jana sotu kāmi.",
  "anya-e0607n:302": "59.",
  "anya-e0607n:303": "Uṇṇā tathābhinava patta varābhi rāmā,",
  "anya-e0607n:304": "Līlollasanta bhamukadvaya nīla pattā;",
  "anya-e0607n:305": "Ghānoru cāru kadalī vadanā lavālā,",
  "anya-e0607n:306": "Tuyhaṃ pavattatu ciraṃ jana maṅgalāya.",
  "anya-e0607n:307": "60.",
  "anya-e0607n:308": "Bālatthalī hari silātala piṭṭhikaṭṭha,",
  "anya-e0607n:309": "Bhūvallaridvaya mayūra yugassa tuyhaṃ;",
  "anya-e0607n:310": "Pañcappabhā rucira piccha yugassirīkaṃ,",
  "anya-e0607n:311": "Nettadvayaṃ manasi puñchatu pāpadhūliṃ.",
  "anya-e0607n:312": "61.",
  "anya-e0607n:313": "Indīvarāntagata bhiṅgika panti bhaṅgi,",
  "anya-e0607n:314": "Pañambujassaratate viya gacchapantī;",
  "anya-e0607n:315": "Nettambujassiri tirokaraṇīva tuyhaṃ,",
  "anya-e0607n:316": "Pamhāvalī sirigateha tiro karontu.",
  "anya-e0607n:317": "62.",
  "anya-e0607n:318": "Vattullasambuja vilocana haṃsa tuṇḍa,",
  "anya-e0607n:319": "Kañjaṃsu piñjara mulāla latā dvyābhaṃ;",
  "anya-e0607n:320": "Dolādvayaṃva savaṇadvyamatta lakkhyā,",
  "anya-e0607n:321": "Hotaṃ tavajja janatā maticārahetu.",
  "anya-e0607n:322": "63.",
  "anya-e0607n:323": "Vammīka matthaka sayānaka bhūridatta,",
  "anya-e0607n:324": "Bhoginda bhogavali vibbhamamā vahanti;",
  "anya-e0607n:325": "Ghānopariṭṭhitamune tava tuṇṇamuṇṇā,",
  "anya-e0607n:326": "Taggāhino viya janassa dadātu vittaṃ.",
  "anya-e0607n:327": "64.",
  "anya-e0607n:328": "Rūpindirāya vijaye khila loka rūpaṃ,",
  "anya-e0607n:329": "Ghāṇoru cāru parighopari baddha siddhā;",
  "anya-e0607n:330": "Nīlābha vāta viluthanta vayaddhajābhā,",
  "anya-e0607n:331": "Tiṭṭhantu sajja duritāri jayāya te bhū.",
  "anya-e0607n:332": "65.",
  "anya-e0607n:333": "Uṇṇassitopala nivesita bunda sandhi,",
  "anya-e0607n:334": "Ghāṇoru piṇḍakamaghā tapa rundhitunte;",
  "anya-e0607n:335": "Hotammukhambuja sirī sirasussitābhaṃ,",
  "anya-e0607n:336": "Bhū nīla paṭṭika lalāta suvaṇṇa chattaṃ.",
  "anya-e0607n:337": "66.",
  "anya-e0607n:338": "Rupaṅka vedana vilocana bāna diṭṭhī,",
  "anya-e0607n:339": "Dhārā nisāna maṇivaṭṭa sirī siro te;",
  "anya-e0607n:340": "Siddhā matosadha katañjana puñja lakkhī,",
  "anya-e0607n:341": "Hotaṃ janassa nayanāmaya nāsanāya.",
  "anya-e0607n:342": "67.",
  "anya-e0607n:343": "Sakkhandha bāhuyuga torana majjha gīvā,",
  "anya-e0607n:344": "Dharappitassirighato pari mussavāya;",
  "anya-e0607n:345": "Nīluppalāva ṭhapitā savibhatti kante,",
  "anya-e0607n:346": "Kesā bhavantu bhuvanattaya maṅgalāya.",
  "anya-e0607n:347": "68.",
  "anya-e0607n:348": "Hemagghiye ṭhapita nīla silā kapāle,",
  "anya-e0607n:349": "Pajjota jāla lalitaṃ muni sārayantī;",
  "anya-e0607n:350": "Rūpassirī sirasi bhūsita hema mālā,",
  "anya-e0607n:351": "Kārā karotu subhagaṃ tava ketu mālā.",
  "anya-e0607n:352": "69.",
  "anya-e0607n:353": "Bhyāmappabhāli tava kañcana mora kāle,",
  "anya-e0607n:354": "Surodaye vitata candaka cakkalakkhī;",
  "anya-e0607n:355": "Meghāvanaddha sikharunnata hema selā,",
  "anya-e0607n:356": "Yantindacāpa vikatīva dadātu sobhaṃ.",
  "anya-e0607n:357": "70.",
  "anya-e0607n:358": "Paṭṭhāya te paṇidhito suci dāna sīla,",
  "anya-e0607n:359": "Nekkhamma pañña viriyakkhama saccadhiṭṭhā;",
  "anya-e0607n:360": "Mettā upekkhiti ime dasa pūratova,",
  "anya-e0607n:361": "Pūrentu pārami guṇā janatānamatte.",
  "anya-e0607n:362": "71.",
  "anya-e0607n:363": "Pattuttaruttaradasā paṇidhāna bījā,",
  "anya-e0607n:364": "Cetoradharāya karuṇā jala sekha vuddhā;",
  "anya-e0607n:365": "Sabbaññu ñāṇa phaladā sati vāṭa guttā,",
  "anya-e0607n:366": "Taṃ samphalandisatu pāramitā latā te.",
  "anya-e0607n:367": "72.",
  "anya-e0607n:368": "Ābodhi puṇṇami padiṭṭha dinādito te,",
  "anya-e0607n:369": "Sambhāra kāla sita pakkha kamābhi vuddho;",
  "anya-e0607n:370": "Sampuṇṇa pārami guṇāmataraṃsi taṃva,",
  "anya-e0607n:371": "Sabbaṅgi kunda kumudāni pabodhayeyya.",
  "anya-e0607n:372": "73.",
  "anya-e0607n:373": "Āpacchimabbhava sivapphala lābha dānā,",
  "anya-e0607n:374": "Dānappabandhamapidāna phalappabhandaṃ;",
  "anya-e0607n:375": "Saṃvaḍḍhayi tvaṃ abhipatthanato yathevaṃ,",
  "anya-e0607n:376": "Jantuttaruttara phalaṃ khalu sambhunantu.",
  "anya-e0607n:377": "74.",
  "anya-e0607n:378": "Ārambhatoppabhuti yāva tavaggamaggā,",
  "anya-e0607n:379": "Vikkhālita ghakalusaṃ suci sīla toyaṃ;",
  "anya-e0607n:380": "Mettā dayā madhura sītalatāyupetaṃ,",
  "anya-e0607n:381": "Sodhetu tvaṃva bhava nissita jantu metaṃ.",
  "anya-e0607n:382": "75.",
  "anya-e0607n:383": "Āpaccimattamabhinikkhamanābhiyogā,",
  "anya-e0607n:384": "Paṭṭhāya tampabhavato paripuṇṇa gehā;",
  "anya-e0607n:385": "Tvaṃ sabba jāti gahato api nikkhamittho,",
  "anya-e0607n:386": "Evaṃ janā bhava dukhā khalu nikkhamantu.",
  "anya-e0607n:387": "76.",
  "anya-e0607n:388": "Ekaggato pala tale nisitā cirandhi,",
  "anya-e0607n:389": "Dhārā sucittu sutale sati daṇḍa baddhe;",
  "anya-e0607n:390": "Nibbijjhi lakkhaṇa dhanuṭṭhiti santi lakkhaṃ,",
  "anya-e0607n:391": "Khittā tayonamanu vijjhatu jantu khittā.",
  "anya-e0607n:392": "77.",
  "anya-e0607n:393": "Tvaṃ pāramī jala nidhiṃ caturiha bāhu,",
  "anya-e0607n:394": "Sattīhi suttari ciraṃ janakova sindhuṃ;",
  "anya-e0607n:395": "Sampanna vikkama phalosi yathā casova,",
  "anya-e0607n:396": "Evaṃ janā viriyatapphalame dhayantu.",
  "anya-e0607n:397": "78.",
  "anya-e0607n:398": "Satta paradha dahanesu ciraṃ sudhantaṃ,",
  "anya-e0607n:399": "Khantī suvaṇṇa kata rūpa samantimattā;",
  "anya-e0607n:400": "Sabbā parādhamasahi tvaṃasayhamevaṃ,",
  "anya-e0607n:401": "Sabbe janāpi khamanena bhajantu santiṃ.",
  "anya-e0607n:402": "79.",
  "anya-e0607n:403": "Lakkhādhikaṃ catura saṃkhiya kappa kālaṃ,",
  "anya-e0607n:404": "Saccena suṭṭhu paribhāvita vācino te;",
  "anya-e0607n:405": "Vācāya sacca phusitāya samenti jantu,",
  "anya-e0607n:406": "Evaṃ visuddha vacanā janatā bhavantu.",
  "anya-e0607n:407": "80.",
  "anya-e0607n:408": "Ādinna dhamma mahiyatthira suppatiṭṭhā,",
  "anya-e0607n:409": "Dhiṭṭhāna pārami mahā vajiraddi tuyhaṃ;",
  "anya-e0607n:410": "Sattena kena pi yathāhi abhejja nejjo,",
  "anya-e0607n:411": "Evaṃ janāpi kusalesu adhiṭṭha hantu.",
  "anya-e0607n:412": "81.",
  "anya-e0607n:413": "Tvaṃ sabba satta cirabhāvita metta citta,",
  "anya-e0607n:414": "Toyehi saṃsamita kodha mahā hutāso;",
  "anya-e0607n:415": "Lokuttaraṃ taditaraṃ hitamāvahittho,",
  "anya-e0607n:416": "Evaṃ janesu janatā hitamāvahantū.",
  "anya-e0607n:417": "82.",
  "anya-e0607n:418": "Mittopakāra paṭipakkha janāpakāre,",
  "anya-e0607n:419": "Tvaṃ nibbikāra manaso cirabhāvanāya;",
  "anya-e0607n:420": "Pattosilābha pabhutaṭṭusu nibbikāraṃ,",
  "anya-e0607n:421": "Evaṃ janānunaya kopa nudā bhavantu.",
  "anya-e0607n:422": "83.",
  "anya-e0607n:423": "Sampanna hetu vibhavo tusite vimānaṃ,",
  "anya-e0607n:424": "Yuttaṃ guṇehi navabhippadavī vimānaṃ;",
  "anya-e0607n:425": "Tvaṃ vādhiparamidhirohiniyā tiloko,",
  "anya-e0607n:426": "Ārohatu bhaya sukhaṃ padavī vimānaṃ.",
  "anya-e0607n:427": "84.",
  "anya-e0607n:428": "Tvaṃverahaṃsi samabujjhi yathāca sammā,",
  "anya-e0607n:429": "Sampanna vijja caraṇo sugatosi hontu;",
  "anya-e0607n:430": "Lokaṃ vido purisadammasusārathī si,",
  "anya-e0607n:431": "Satthāsi bujjhi bhagavā si tatheva jantu.",
  "anya-e0607n:432": "85.",
  "anya-e0607n:433": "Saccitta bhū nidahitaṃ janatāya tuyhaṃ,",
  "anya-e0607n:434": "Kalyāṇavaṇṇaratanaṇṇavajātibhinnaṃ;",
  "anya-e0607n:435": "Dukkhaggi cora jalupaddutajāti gehe,",
  "anya-e0607n:436": "Tassā sukhaṃ bhavatu jīvitumāpadāya.",
  "anya-e0607n:437": "86.",
  "anya-e0607n:438": "Vācā vicitta vara tantu gataṅgi kaṇṭhe,",
  "anya-e0607n:439": "Svā mutta sagguṇa mahā ratanā valī te;",
  "anya-e0607n:440": "Vevaṇṇi yattani bhavaṃ sakalampahāya,",
  "anya-e0607n:441": "Hotañjanassa siri saṅgama maṅgalāya.",
  "anya-e0607n:442": "87.",
  "anya-e0607n:443": "Taṃ sagguṇatthava dahaṭṭha sutippanāli,",
  "anya-e0607n:444": "Nissandamāna guṇanīra nipāna tinte;",
  "anya-e0607n:445": "Khettetta saññini janā kata loma haṃsa,",
  "anya-e0607n:446": "Bījankurī kusala sassa phalaṃ labhantu.",
  "anya-e0607n:447": "88.",
  "anya-e0607n:448": "Āpāyikappabhuti dukkha nidāgha kāla,",
  "anya-e0607n:449": "Santāpitā nikhila loka mano kadambā;",
  "anya-e0607n:450": "Taṃ vaṅṇa megha phusanā hasanaṅkurehi,",
  "anya-e0607n:451": "Iddhā bhavantu mati vallari vellītā te.",
  "anya-e0607n:452": "89.",
  "anya-e0607n:453": "Hetuddasā phaladasā samavaṭṭhitaṃ taṃ,",
  "anya-e0607n:454": "Sabbattha satta hitamāvahaṇena siddhaṃ;",
  "anya-e0607n:455": "Cintāpathātiganubhāva vibhāvanante,",
  "anya-e0607n:456": "Bhūtānamatthu caritabbhutamattha sidhyā.",
  "anya-e0607n:457": "90.",
  "anya-e0607n:458": "Aṅgārakāsumabhilaṅghiya dāna kāle,",
  "anya-e0607n:459": "Bhattattano pada paṭicchaka paṅkajā ca;",
  "anya-e0607n:460": "Yātakkhaṇe tava pade dhaṭamuṭṭhahitvā,",
  "anya-e0607n:461": "Paṅkeruhāṃ siva madhuṃ sarataṃ dadantu.",
  "anya-e0607n:462": "91.",
  "anya-e0607n:463": "Saccena maccha pati vassita vassadhārā,",
  "anya-e0607n:464": "Satte dayāya tava vassita vassadhārā;",
  "anya-e0607n:465": "Gimhe janassa samayiṃsu yathā tathātā,",
  "anya-e0607n:466": "Dhamambuvuṭṭhiva samentu kilesa dāhe.",
  "anya-e0607n:467": "92.",
  "anya-e0607n:468": "Chaddanta nāga patinā khamatā parādhaṃ,",
  "anya-e0607n:469": "Chetvā kare ṭhapita dantavarāva luddaṃ;",
  "anya-e0607n:470": "Loke hitāya ṭhapitā tava danta dhātu,",
  "anya-e0607n:471": "Seṭṭhā janaṃ siva puraṃ lahu pāpayantu.",
  "anya-e0607n:472": "93.",
  "anya-e0607n:473": "Taṃ temiyākhya yatinossama mālakamhi,",
  "anya-e0607n:474": "Okiṇṇa mutta kanakā vuja vippakiṇṇā;",
  "anya-e0607n:475": "Kāruñña vārida cuto daka bindu bandhū,",
  "anya-e0607n:476": "Dhātu samentu tava jantusu dukkhadāhe.",
  "anya-e0607n:477": "94.",
  "anya-e0607n:478": "Raṭṭhassa attha caraṇāya asammukhassa,",
  "anya-e0607n:479": "Rāmena dinna tiṇa saṃkhata pādukāva;",
  "anya-e0607n:480": "Bhuttā tayā ciramasammukha nāgatassa,",
  "anya-e0607n:481": "Lokassa atthamanu tiṭṭhatu patta dhātu.",
  "anya-e0607n:482": "95.",
  "anya-e0607n:483": "Vutto janānamupadissa varāha raññā,",
  "anya-e0607n:484": "Satthiṃ sahassa saradaṃ viya ñāya dhammo;",
  "anya-e0607n:485": "Ādeyya heyyamupadissa tayā pavutto,",
  "anya-e0607n:486": "Dhammo pavattatu ciraṃ janatā hitāya.",
  "anya-e0607n:487": "96.",
  "anya-e0607n:488": "Mārāri maddana hitādhigamaṃ karotā,",
  "anya-e0607n:489": "Bhatto tayā vara mahā jaya bodhi rājā;",
  "anya-e0607n:490": "Saggā pavavagga hitahetu janassa hantvā,",
  "anya-e0607n:491": "Sabbantarāyamiha tiṭṭhatu suṭṭhu sajjo.",
  "anya-e0607n:492": "97.",
  "anya-e0607n:493": "Sāmoda vaṇṇa bhajanī guṇa mañjarīyaṃ,",
  "anya-e0607n:494": "Cariyā latā vikasitā tava sapphalaṅgaṃ;",
  "anya-e0607n:495": "Okiṇṇa citta madhupe rasa pīṇayanti,",
  "anya-e0607n:496": "Sambhāvitā bhuvi pavattatu matthakehi.",
  "anya-e0607n:497": "98.",
  "anya-e0607n:498": "Sambuddha selavalayantara jānanavhā,",
  "anya-e0607n:499": "Nottattato tipathagā yati sāgaraṭṭhā;",
  "anya-e0607n:500": "Dhammā pagā suti vase tarite puṇanti,",
  "anya-e0607n:501": "Sambhāra sassamiha vattatu pacayanti.",
  "anya-e0607n:502": "99.",
  "anya-e0607n:503": "Paññāṇa kūpa sita paggaha vāyu gāhī,",
  "anya-e0607n:504": "Saddhā lakāra sahitā sati pota vāhā;",
  "anya-e0607n:505": "Sampāpayātu bhava sāgara pāra tīra,",
  "anya-e0607n:506": "Sappattanaṃ varadhane pati patti nāvā.",
  "anya-e0607n:507": "100.",
  "anya-e0607n:508": "Bojjhaṅga satta ratanākara dhamma khandha,",
  "anya-e0607n:509": "Gambhīra nīra caya sāsana sāgaro saṃ;",
  "anya-e0607n:510": "So sīlyananta tanu veṭitha ñāṇa mantha,",
  "anya-e0607n:511": "Selena manthitavataṃ disatā mataṃ ve.",
  "anya-e0607n:512": "101.",
  "anya-e0607n:513": "Vuttena tena vidhinā vidhinā tato taṃ,",
  "anya-e0607n:514": "Laddhā nubhūtamamataṃ khila dosa nāsaṃ;",
  "anya-e0607n:515": "Accanta roga jaratā maraṇā bhi bhūtaṃ,",
  "anya-e0607n:516": "Bhūtaṃ karotu amaraṃ ajaraṃ arogaṃ.",
  "anya-e0607n:517": "102.",
  "anya-e0607n:518": "Saddhamma rāja raviniggata dhammaraṃsi,",
  "anya-e0607n:519": "Phullo dhutaṅgadala saṃvara kesarāli;",
  "anya-e0607n:520": "Saṅghāravinda nikaro samadhuṃ samādhi,",
  "anya-e0607n:521": "Sakkiṇṇiko disatu sāsana vāpi jato.",
  "anya-e0607n:522": "103.",
  "anya-e0607n:523": "Ānanda rañña ratanādi mahā yatinda,",
  "anya-e0607n:524": "Niccappabuddha padumappiya sevinaṅgī;",
  "anya-e0607n:525": "Buddhappiyena ghana buddha guṇappiyena,",
  "anya-e0607n:526": "Therālinā racita pajjamadhuṃ pibantu.",
  "anya-e0607n:527": "104.",
  "anya-e0607n:528": "Itthaṃ rūpa guṇānukittanavasā taṃ taṃ hitā siṃ sato,",
  "anya-e0607n:529": "Vatthānussati vattita iha yathā sattesu mettā ca me;",
  "anya-e0607n:530": "Evaṃ tābhi bhavanta ruttara tarā vattantu tā bodhi me,",
  "anya-e0607n:531": "Saṃyogoca dhanehi santihi bhave kalyāna mittehi ca."
}