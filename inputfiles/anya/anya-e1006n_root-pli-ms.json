{
  "anya-e1006n:0": " Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1006n:1": "Suttantanīti",
  "anya-e1006n:2": "1.",
  "anya-e1006n:3": " Parā bhavantaṃ purisaṃ,",
  "anya-e1006n:4": "Mayaṃ pucchāma gotamaṃ;",
  "anya-e1006n:5": "Bhavantaṃ puṭṭhu māgamma,",
  "anya-e1006n:6": "Kiṃ parābhavato mukhaṃ.",
  "anya-e1006n:7": "2.",
  "anya-e1006n:8": " Suvijāno bhavaṃhoti,",
  "anya-e1006n:9": "Duvijāno parābhavo;",
  "anya-e1006n:10": "Dhammakāmo bhavaṃ hoti,",
  "anya-e1006n:11": "Dhammadessī parābhavo.",
  "anya-e1006n:12": "3.",
  "anya-e1006n:13": " Itihe taṃ vijānāma,",
  "anya-e1006n:14": "Pathamo so parābhavo;",
  "anya-e1006n:15": "Dutīyaṃ bhagavā brūhi,",
  "anya-e1006n:16": "Kiṃ parābhavato mukhaṃ.",
  "anya-e1006n:17": "4.",
  "anya-e1006n:18": " Asantassa piyo hoti,",
  "anya-e1006n:19": "Sante na kurute piyaṃ;",
  "anya-e1006n:20": "Asataṃ dhammaṃ roceti,",
  "anya-e1006n:21": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:22": "Kammāparādhasattānaṃ,",
  "anya-e1006n:23": "Vināse paccupaṭṭhite;",
  "anya-e1006n:24": "Anayo nayarūpena,",
  "anya-e1006n:25": "Buddhimākamya tiṭṭhati.",
  "anya-e1006n:26": "5.",
  "anya-e1006n:27": " Niddāsīlī sabhāsīlī,",
  "anya-e1006n:28": "Anuṭṭhātā ca yo naro;",
  "anya-e1006n:29": "Alaso kodhapaññāṇo,",
  "anya-e1006n:30": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:31": "6.",
  "anya-e1006n:32": " Yo mātaraṃ pitaraṃ vā,",
  "anya-e1006n:33": "Jiṇṇakaṃ gatayobbanaṃ;",
  "anya-e1006n:34": "Pahusanto na bharati,",
  "anya-e1006n:35": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:36": "7.",
  "anya-e1006n:37": " Yo brāhmaṇaṃ samaṇaṃ vā,",
  "anya-e1006n:38": "Aññaṃ vāpi vaṇibbakaṃ;",
  "anya-e1006n:39": "Musāvādena vañceti,",
  "anya-e1006n:40": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:41": "8.",
  "anya-e1006n:42": " Pahutavitto puriso,",
  "anya-e1006n:43": "Sahirañño sabhojano;",
  "anya-e1006n:44": "Eko bhuñjati sādūni,",
  "anya-e1006n:45": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:46": "9.",
  "anya-e1006n:47": " Jātithaddho dhanathaddho,",
  "anya-e1006n:48": "Gottathaddho ca yo naro;",
  "anya-e1006n:49": "Sañātiṃ atimaññeti,",
  "anya-e1006n:50": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:51": "10.",
  "anya-e1006n:52": " Itthidhutto surādhutto,",
  "anya-e1006n:53": "Akkhadhutto ca yo naro;",
  "anya-e1006n:54": "Laddhaṃladdhaṃ vināseti,",
  "anya-e1006n:55": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:56": "Na bāḷhaṃ itthiṃ gaccheyya,",
  "anya-e1006n:57": "Sampassaṃ tejasaṅkhayaṃ;",
  "anya-e1006n:58": "Kāsaṃ sāsaṃ daraṃ bālyaṃ,",
  "anya-e1006n:59": "Khīṇamedo nigacchati.",
  "anya-e1006n:60": "(Ka)",
  "anya-e1006n:61": "Māyācetā marīcī ca,",
  "anya-e1006n:62": "Soko rogo upaddavo;",
  "anya-e1006n:63": "Kharā ca bandhanācetā,",
  "anya-e1006n:64": "Maccupāso guhāsayo.",
  "anya-e1006n:65": "(Kha)",
  "anya-e1006n:66": "Balavanto dubbalā honti,",
  "anya-e1006n:67": "Thāmavantopi hāyare;",
  "anya-e1006n:68": "Cakkhumā andhakā honti,",
  "anya-e1006n:69": "Mātugāmavasaṃgatā.",
  "anya-e1006n:70": "(Ga)",
  "anya-e1006n:71": "Guṇavanto nigguṇā honti,",
  "anya-e1006n:72": "Paññavantopi hāyare;",
  "anya-e1006n:73": "Pamuttā bandhanā senti,",
  "anya-e1006n:74": "Mātugāmavasaṃgatā.",
  "anya-e1006n:75": "(Gha)",
  "anya-e1006n:76": "Yasaṃ kittiṃ dhitiṃ sūraṃ;",
  "anya-e1006n:77": "Bāhussaccaṃ pajānanaṃ;",
  "anya-e1006n:78": "Hāpayanti pamattassa;",
  "anya-e1006n:79": "Kaṭṭhapuñcaṃva pāvako.",
  "anya-e1006n:80": "11.",
  "anya-e1006n:81": " Sehi dārehi santuṭṭho,",
  "anya-e1006n:82": "Vesiyāsu padussati;",
  "anya-e1006n:83": "Dussati paradāresu,",
  "anya-e1006n:84": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:85": "(Ka)",
  "anya-e1006n:86": "Mayañca bhariyaṃ nātikkamāma,",
  "anya-e1006n:87": "Amheca bhariyā nātikkamanti;",
  "anya-e1006n:88": "Aññatra tāhi brahmacariyaṃ carāma,",
  "anya-e1006n:89": "Tasmā hi amhaṃ daharā na miyyare.",
  "anya-e1006n:90": "(Kha)",
  "anya-e1006n:91": "Etāsu jāyare suttamāsu,",
  "anya-e1006n:92": "Medhāvino honti pahutapaññā;",
  "anya-e1006n:93": "Bahussutā theraguṇā ca honti,",
  "anya-e1006n:94": "Tasmā hi amhaṃ daharā na miyyare.",
  "anya-e1006n:95": "12.",
  "anya-e1006n:96": " Atītayobbano poso,",
  "anya-e1006n:97": "Āneti timbarutthaniṃ;",
  "anya-e1006n:98": "Tassa issā na supati,",
  "anya-e1006n:99": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:100": "(Ka)",
  "anya-e1006n:101": "Na dukkhaṃ ahinā daṭṭhaṃ,",
  "anya-e1006n:102": "Na dukkhaṃ sattiyā hataṃ;",
  "anya-e1006n:103": "Tañca dukkhañca tibbañca,",
  "anya-e1006n:104": "Yaṃ passe jiṇṇakaṃ patiṃ.",
  "anya-e1006n:105": "(Kha)",
  "anya-e1006n:106": "Natthi khiṭṭā natthi rati,",
  "anya-e1006n:107": "Jiṇṇena patinā saha;",
  "anya-e1006n:108": "Natthi allāpasallāpo,",
  "anya-e1006n:109": "Jagghituṃpi na sobhati.",
  "anya-e1006n:110": "(Ga)",
  "anya-e1006n:111": "Yadā ca daharo daharī,",
  "anya-e1006n:112": "Mantayiṃsu rahogatā;",
  "anya-e1006n:113": "Sabbe sokā vinassanti,",
  "anya-e1006n:114": "Yekeci hadayassitā.",
  "anya-e1006n:115": "13.",
  "anya-e1006n:116": " Itthiṃ soṇḍiṃ vikiraṇiṃ,",
  "anya-e1006n:117": "Purisaṃ vāpi tādisaṃ;",
  "anya-e1006n:118": "Issariyasmiṃ thapeti,",
  "anya-e1006n:119": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:120": "14.",
  "anya-e1006n:121": " Appabhogo mahātaṇho,",
  "anya-e1006n:122": "Khattiye jāyate kule;",
  "anya-e1006n:123": "So ca rajjaṃ patthayati,",
  "anya-e1006n:124": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:125": "‘‘Suvijāno bhavaṃhoti,",
  "anya-e1006n:126": "Duvijāno parābhavo’’;",
  "anya-e1006n:127": "Appabhogo mahātaṇho.",
  "anya-e1006n:128": "15.",
  "anya-e1006n:129": " Ete parābhave loke,",
  "anya-e1006n:130": "Paṇḍito samavekkhiya;",
  "anya-e1006n:131": "Ariyo dassanasampanno,",
  "anya-e1006n:132": "Sa lokaṃ bhajate sivaṃ.",
  "anya-e1006n:133": "Vasalasutta",
  "anya-e1006n:134": "1.",
  "anya-e1006n:135": " Kodhano upanāhīca,",
  "anya-e1006n:136": "Pāpamakkhī ca yo naro;",
  "anya-e1006n:137": "Vipannadiṭṭhī māyāvī,",
  "anya-e1006n:138": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:139": "2.",
  "anya-e1006n:140": " Ekajaṃ vā dvijaṃ vāpi,",
  "anya-e1006n:141": "Yodha pāṇaṃ vihiṃsati;",
  "anya-e1006n:142": "Yassa pāṇe dayā natthi,",
  "anya-e1006n:143": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:144": "3.",
  "anya-e1006n:145": " Yo hanti uparundhati,",
  "anya-e1006n:146": "Gāmāni nigamāni ca;",
  "anya-e1006n:147": "Niggāhako samaññāto,",
  "anya-e1006n:148": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:149": "4.",
  "anya-e1006n:150": " Gāme vā yadi vā raññe,",
  "anya-e1006n:151": "Yaṃ paresaṃ mamāyitaṃ;",
  "anya-e1006n:152": "Theyyā adinnaṃ ādeti,",
  "anya-e1006n:153": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:154": "5.",
  "anya-e1006n:155": " Yo have iṇamādāya,",
  "anya-e1006n:156": "Vuccamāno palāyati;",
  "anya-e1006n:157": "Na hi te iṇamatthīti,",
  "anya-e1006n:158": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:159": "6.",
  "anya-e1006n:160": " Yodha kiñcikkhakamyatā,",
  "anya-e1006n:161": "Pathasmiṃ vajataṃ janaṃ;",
  "anya-e1006n:162": "Hantvā kiñcikkha mādeti;",
  "anya-e1006n:163": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:164": "7.",
  "anya-e1006n:165": " Yo attahetu parahetu,",
  "anya-e1006n:166": "Dhanahetu ca yo naro;",
  "anya-e1006n:167": "Sakkhipuṭṭho musābrūti,",
  "anya-e1006n:168": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:169": "8.",
  "anya-e1006n:170": " Yo ñātīnaṃ sakhīnaṃ vā,",
  "anya-e1006n:171": "Dāresu paṭidissati;",
  "anya-e1006n:172": "Sahasā sampiyena vā,",
  "anya-e1006n:173": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:174": "9.",
  "anya-e1006n:175": " Yo 4 mātaraṃ pitaraṃ vā,",
  "anya-e1006n:176": "Jiṇṇakaṃ gatayobbanaṃ;",
  "anya-e1006n:177": "Pahusanto na bharati,",
  "anya-e1006n:178": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:179": "10.",
  "anya-e1006n:180": " Yo mātaraṃ pitaraṃ vā,",
  "anya-e1006n:181": "Bhātaraṃ bhaginiṃ sassuṃ;",
  "anya-e1006n:182": "Hanti roseti vācāya,",
  "anya-e1006n:183": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:184": "11.",
  "anya-e1006n:185": " Yo atthaṃ pucchito santo,",
  "anya-e1006n:186": "Anattha manusāsati;",
  "anya-e1006n:187": "Paṭicchannena manteti,",
  "anya-e1006n:188": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:189": "12.",
  "anya-e1006n:190": " Yo katvā pāpakaṃ kammaṃ,",
  "anya-e1006n:191": "Māmaṃ jaññāti icchati;",
  "anya-e1006n:192": "Yo paṭicchannakammanto,",
  "anya-e1006n:193": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:194": "13.",
  "anya-e1006n:195": " Yo ve parakulaṃ gantvā,",
  "anya-e1006n:196": "Bhutvāna sucibhojanaṃ;",
  "anya-e1006n:197": "Āgataṃ nappaṭipūjeti,",
  "anya-e1006n:198": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:199": "14.",
  "anya-e1006n:200": " Yo samaṇaṃ vā brāhmaṇaṃ,",
  "anya-e1006n:201": "Aññaṃ vāpi vaṇibbakaṃ;",
  "anya-e1006n:202": "Musāvādena vañceti,",
  "anya-e1006n:203": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:204": "15.",
  "anya-e1006n:205": " Yo samaṇaṃ vā brāhmaṇaṃ,",
  "anya-e1006n:206": "Bhattakāle upaṭṭhitaṃ;",
  "anya-e1006n:207": "Roseti vā na ca deti,",
  "anya-e1006n:208": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:209": "16.",
  "anya-e1006n:210": " Asantaṃ yodha pabrūti,",
  "anya-e1006n:211": "Mohena paliguṇṭhito;",
  "anya-e1006n:212": "Kiñcanaṃ nijigīsāno,",
  "anya-e1006n:213": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:214": "17.",
  "anya-e1006n:215": " Yocattānaṃ samukkaṃse,",
  "anya-e1006n:216": "Pareca mavajānāti;",
  "anya-e1006n:217": "Nihīno sena mānena,",
  "anya-e1006n:218": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:219": "18.",
  "anya-e1006n:220": " Rosako kadarīyo ca,",
  "anya-e1006n:221": "Pāpiccho maccharī saṭho;",
  "anya-e1006n:222": "Ahirīko anottappī,",
  "anya-e1006n:223": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:224": "19.",
  "anya-e1006n:225": " Yo buddhaṃ paribhāsati,",
  "anya-e1006n:226": "Athavā tassa sāvakaṃ;",
  "anya-e1006n:227": "Paribbajaṃ gahaṭṭhaṃ vā,",
  "anya-e1006n:228": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:229": "Aṭṭhahi bhikkhave aṅgehi sampannāgatassa upāsakassa patto nikujjitabbo. Bhikkhūnaṃ alābhāya parisakkati, bhikkhūnaṃ anatthāya parisakkati, bhikkhūnaṃ anāvāsāya parisakkati, bhikkhūnaṃ akkosati paribhāsati, bhikkhūbhikkhūhi bhedeti, buddhassa avaṇṇaṃ bhāsati, dhammassa avaṇṇaṃ bhāsati, saṅghassa avaṇṇaṃ bhāsati, anujānāmi bhikkhave imehi aṭṭhahi aṅgehi sampannāgatassa upāsakassa pattaṃ nikujjituṃ.",
  "anya-e1006n:230": "20.",
  "anya-e1006n:231": " Yo anarahaṃ santo,",
  "anya-e1006n:232": "Arahāti paṭijānāti;",
  "anya-e1006n:233": "Coro sabrahmake loke,",
  "anya-e1006n:234": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:235": "21.",
  "anya-e1006n:236": " Ete kho vasalā vuttā,",
  "anya-e1006n:237": "Mayā ye te pakāsitā;",
  "anya-e1006n:238": "Na jaccā vasalo hoti,",
  "anya-e1006n:239": "Na jaccā hoti brāhmaṇo.",
  "anya-e1006n:240": "22.",
  "anya-e1006n:241": " Kammunā vasalo hoti,",
  "anya-e1006n:242": "Kammunā hoti brāhmaṇo.",
  "anya-e1006n:243": "23.",
  "anya-e1006n:244": " Tadamināpi jānātha,",
  "anya-e1006n:245": "Yathāhetaṃ nidassanaṃ;",
  "anya-e1006n:246": "Caṇḍālaputto sopāko,",
  "anya-e1006n:247": "Mātaṅgo iti vissuto.",
  "anya-e1006n:248": "24.",
  "anya-e1006n:249": " So yasaṃ paramaṃ patto,",
  "anya-e1006n:250": "Mātaṅgo yaṃ sudullabhaṃ;",
  "anya-e1006n:251": "Āgacchuṃ tassupaṭṭhānaṃ,",
  "anya-e1006n:252": "Khattiyā brāhmaṇā bahū.",
  "anya-e1006n:253": "25.",
  "anya-e1006n:254": " So devayānaṃ abhiruyha,",
  "anya-e1006n:255": "Virajaṃ so mahāpathaṃ;",
  "anya-e1006n:256": "Kāmarāgaṃ virājetvā,",
  "anya-e1006n:257": "Brahmalokūpago ahu.",
  "anya-e1006n:258": "Giriṃ nakhena khaṇasi,",
  "anya-e1006n:259": "Ayo dantebhi khādasi;",
  "anya-e1006n:260": "Jātavedaṃ padahasi,",
  "anya-e1006n:261": "Yo isiṃ paribhāsati.",
  "anya-e1006n:262": "Āvelitaṃ piṭṭhito uttamaṅgaṃ,",
  "anya-e1006n:263": "Bāhuṃ pasāreti akampaṇeyyaṃ;",
  "anya-e1006n:264": "Setāni akkhīni yathā matassa,",
  "anya-e1006n:265": "Ko me imaṃ puttamakāsi evaṃ."
}