{
  "anya-e1002n0:0": " Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1002n0:1": "Nītimañjarī",
  "anya-e1002n0:2": "1.",
  "anya-e1002n0:3": " Kulakkhaye vinassanti,",
  "anya-e1002n0:4": "Kuladhammā sanantanā;",
  "anya-e1002n0:5": "Dhamme naṭṭhe kulaṃ sabbaṃ,",
  "anya-e1002n0:6": "Adhammo abhibhū khalaṃ.",
  "anya-e1002n0:7": "2.",
  "anya-e1002n0:8": " Adhammābhibhavā dantā,",
  "anya-e1002n0:9": "Padussanti kulitthiyo;",
  "anya-e1002n0:10": "Thīsu duṭṭhā sva dhammena,",
  "anya-e1002n0:11": "Jāyate vaṇṇasaṅkaro.",
  "anya-e1002n0:12": "3.",
  "anya-e1002n0:13": " Piyaṃ bhāse guṇaggāho,",
  "anya-e1002n0:14": "Sūro siyā vikantano;",
  "anya-e1002n0:15": "Dātā candasamā nārī,",
  "anya-e1002n0:16": "Diṭṭhaṃ diṭṭhaṃ nahāsaye.",
  "anya-e1002n0:17": "4.",
  "anya-e1002n0:18": " Kutotthi kumitte saccaṃ,",
  "anya-e1002n0:19": "Kudāre rativaḍḍhanaṃ;",
  "anya-e1002n0:20": "Kudesamhi mano rammaṃ,",
  "anya-e1002n0:21": "Kurāje bhogasampadaṃ.",
  "anya-e1002n0:22": "Saṅketeva amittasmiṃ,",
  "anya-e1002n0:23": "Mittasmiṃ pi navissase;",
  "anya-e1002n0:24": "Abhayā bhaya muppannaṃ,",
  "anya-e1002n0:25": "Api mūlāni kantati.",
  "anya-e1002n0:26": "Adiṭṭhova paro seyyo,",
  "anya-e1002n0:27": "Dummitto no vissāsiko.",
  "anya-e1002n0:28": "Aggihomaphalaṃ vedo,",
  "anya-e1002n0:29": "Satthaṃsīlaphalaṃ mataṃ;",
  "anya-e1002n0:30": "Ratiputtaphalaṃ nārī,",
  "anya-e1002n0:31": "Dānabhuttiphalaṃ dhanaṃ.",
  "anya-e1002n0:32": "Asaccaṃ sāhasaṃ māyā,",
  "anya-e1002n0:33": "Mūḷhatta ma tilobhatā;",
  "anya-e1002n0:34": "Asocaṃ niddayattañca,",
  "anya-e1002n0:35": "Thīnaṃ dosā sabhāvajā.",
  "anya-e1002n0:36": "Jāyāya bhattuno bhāro,",
  "anya-e1002n0:37": "Sissena guruno kato;",
  "anya-e1002n0:38": "Amaccakehi rājassa,",
  "anya-e1002n0:39": "Pitarānaṃ nijenaca.",
  "anya-e1002n0:40": "5.",
  "anya-e1002n0:41": " Uyyamena hi sijjhanti,",
  "anya-e1002n0:42": "Kammāni na manorathā;",
  "anya-e1002n0:43": "Na hi suttassa sīhassa,",
  "anya-e1002n0:44": "Pavīsanti mukhe migā.",
  "anya-e1002n0:45": "Atisītaṃ atiuṇhaṃ,",
  "anya-e1002n0:46": "Atisāyamidaṃ ahu;",
  "anya-e1002n0:47": "Iti visaṭṭhakammante,",
  "anya-e1002n0:48": "Khaṇā accenti māṇave.",
  "anya-e1002n0:49": "Ādānassa padānassa,",
  "anya-e1002n0:50": "Kattabbassa ca kammuno;",
  "anya-e1002n0:51": "Khippaṃ akayyamānassa,",
  "anya-e1002n0:52": "Kāle pivati sampadaṃ.",
  "anya-e1002n0:53": "Nādabbe nihitā kāci,",
  "anya-e1002n0:54": "Kriyā phalavatī bhave;",
  "anya-e1002n0:55": "Nabyāpārasatenāpi,",
  "anya-e1002n0:56": "Sukova pāṭhate bako.",
  "anya-e1002n0:57": "Yo dandhakāle tarati,",
  "anya-e1002n0:58": "Taraṇīye ca dandhaye;",
  "anya-e1002n0:59": "Sukkhapaṇṇaṃ va akkamma,",
  "anya-e1002n0:60": "Atthaṃ bhañjati attano.",
  "anya-e1002n0:61": "6.",
  "anya-e1002n0:62": " Yaṃ dadāti yaṃ bhuñjati,",
  "anya-e1002n0:63": "Tadeva dhanino dhanaṃ;",
  "anya-e1002n0:64": "Aññe matassa kīḷanti,",
  "anya-e1002n0:65": "Dārehipi dhanehipi.",
  "anya-e1002n0:66": "Dānopabhogahīnena,",
  "anya-e1002n0:67": "Dhanena dhanino sukhaṃ;",
  "anya-e1002n0:68": "Ko viseso daliddassa,",
  "anya-e1002n0:69": "Adhikaṃ dhanarakkhaṇaṃ.",
  "anya-e1002n0:70": "Nijasokhyaṃ nirundhanto,",
  "anya-e1002n0:71": "Nīcabhogo mitampaco;",
  "anya-e1002n0:72": "Dhanaṃ sañcayate yo so,",
  "anya-e1002n0:73": "Parabhāravaho pasu.",
  "anya-e1002n0:74": "Yaṃ ussukā saṅkharonti,",
  "anya-e1002n0:75": "Alakkhikā bahuṃ dhanaṃ;",
  "anya-e1002n0:76": "Sippavanto asippāvā,",
  "anya-e1002n0:77": "Lakkhi vā tāni bhuñjati.",
  "anya-e1002n0:78": "7.",
  "anya-e1002n0:79": " Sampatyaṃ mahataṃ cittaṃ,",
  "anya-e1002n0:80": "Bhave uppale komalaṃ;",
  "anya-e1002n0:81": "Vipatyaṃca mahāsela,",
  "anya-e1002n0:82": "Silāsaṅghātakakkasaṃ.",
  "anya-e1002n0:83": "8.",
  "anya-e1002n0:84": " Asambhabyaguṇaṃ thutvā,",
  "anya-e1002n0:85": "Khedo mudhāva jāyate;",
  "anya-e1002n0:86": "Avhāyaṃ canda mu llokya,",
  "anya-e1002n0:87": "Nacandota mu pāgamī.",
  "anya-e1002n0:88": "9.",
  "anya-e1002n0:89": " Saccaṃ mukhamhi dhāreyya,",
  "anya-e1002n0:90": "Kaṇṇe sutaṃ bhuje jayaṃ;",
  "anya-e1002n0:91": "Hadayamhi khamaṃ vīraṃ,",
  "anya-e1002n0:92": "Lokādāsaṃca locane.",
  "anya-e1002n0:93": "Saddamattaṃ naphandeyya,",
  "anya-e1002n0:94": "Aññatvā saddakāraṇaṃ;",
  "anya-e1002n0:95": "Saddahetuṃ pariññāya,",
  "anya-e1002n0:96": "Pamodo vā bhayo tathā.",
  "anya-e1002n0:97": "Sabbasuta ma dhīyeyya,",
  "anya-e1002n0:98": "Hīnamukkaṭṭhamajjhimaṃ.",
  "anya-e1002n0:99": "10.",
  "anya-e1002n0:100": " Dunnāriyā kulaṃ suddhaṃ,",
  "anya-e1002n0:101": "Putto nassati lālanā;",
  "anya-e1002n0:102": "Samiddhi anayā bandhu,",
  "anya-e1002n0:103": "Pavāsā madanā hirī.",
  "anya-e1002n0:104": "Lālaye pañcavassāni,",
  "anya-e1002n0:105": "Dasavassāni tālaye;",
  "anya-e1002n0:106": "Pattetu soḷasevasse,",
  "anya-e1002n0:107": "Puttaṃ mittaṃva ācare.",
  "anya-e1002n0:108": "Lālane bahavo dosā,",
  "anya-e1002n0:109": "Lālane bahavo guṇā.",
  "anya-e1002n0:110": "Pāpā nivārayati yojayate hitāya,",
  "anya-e1002n0:111": "Guyhāni gūhati guṇaṃ pakaṭīkaroti;",
  "anya-e1002n0:112": "Āpattikañca najahāti dadāti kāle,",
  "anya-e1002n0:113": "Sammitta lakkhaṇamidaṃ pavadanti santo.",
  "anya-e1002n0:114": "11.",
  "anya-e1002n0:115": " Dujjano jīyate yutyā,",
  "anya-e1002n0:116": "Niggahena nadhīmatā;",
  "anya-e1002n0:117": "Nipātyate mahārukkho,",
  "anya-e1002n0:118": "Tassamīpa khatikkhayā.",
  "anya-e1002n0:119": "Vane migāca luddhānaṃ,",
  "anya-e1002n0:120": "Dujjanānañca sajjanā;",
  "anya-e1002n0:121": "Akāraṇaverī honti,",
  "anya-e1002n0:122": "Tiṇabhakkhā supesalā.",
  "anya-e1002n0:123": "Pādalaggaṃ karaṭṭhena,",
  "anya-e1002n0:124": "Kaṇḍakeneva kaṇḍakaṃ.",
  "anya-e1002n0:125": "Bālaṃ napasse nasuṇe,",
  "anya-e1002n0:126": "Nacabālena saṃvase;",
  "anya-e1002n0:127": "Bālenāllāpasallāpaṃ,",
  "anya-e1002n0:128": "Nakare nacarocaye.",
  "anya-e1002n0:129": "12.",
  "anya-e1002n0:130": " Upa kattuṃ yathā khuddo,",
  "anya-e1002n0:131": "Samattho natathāmahā;",
  "anya-e1002n0:132": "Kūpo hi hanti pipāsaṃ,",
  "anya-e1002n0:133": "Natu pāyo mahambudhi.",
  "anya-e1002n0:134": "13.",
  "anya-e1002n0:135": " Ādānassa padānassa,",
  "anya-e1002n0:136": "Kattabbassaca kammuno;",
  "anya-e1002n0:137": "Khippaṃ akaramānassa,",
  "anya-e1002n0:138": "Kālo bhakkhati taṃ rasaṃ.",
  "anya-e1002n0:139": "Nakkhattaṃ paṭimānentaṃ,",
  "anya-e1002n0:140": "Attho bālaṃ upajjhagā;",
  "anya-e1002n0:141": "Attho atthassa nakkhattaṃ,",
  "anya-e1002n0:142": "Kiṃ karissanti tārakā.",
  "anya-e1002n0:143": "Ajarāmarova pañño,",
  "anya-e1002n0:144": "Vijjamatthañca cintaye;",
  "anya-e1002n0:145": "Gahitoviya kesesu,",
  "anya-e1002n0:146": "Maccunā dhammamācare.",
  "anya-e1002n0:147": "14.",
  "anya-e1002n0:148": " Vajjā gurūca mantīca,",
  "anya-e1002n0:149": "Tayo raṭṭhābhisaṅkhatā;",
  "anya-e1002n0:150": "Jīvīta dakkha kosānaṃ,",
  "anya-e1002n0:151": "Vaḍḍhanā nāsanāca te.",
  "anya-e1002n0:152": "15.",
  "anya-e1002n0:153": " Thirena kammaṃ vaḍḍhati,",
  "anya-e1002n0:154": "Athirena turena no;",
  "anya-e1002n0:155": "Phalanti samaye rukkhā,",
  "anya-e1002n0:156": "Sittāpi bahuvārinā.",
  "anya-e1002n0:157": "Vāyāmetheva puriso,",
  "anya-e1002n0:158": "Nanibbindeyya paṇḍito.",
  "anya-e1002n0:159": "Payatano tādiso neva,",
  "anya-e1002n0:160": "Kayyo yena phalaṃ nahi;",
  "anya-e1002n0:161": "Selagge kūpakhaṇanā,",
  "anya-e1002n0:162": "Kathaṃ toyasamāgamo.",
  "anya-e1002n0:163": "Ñāṇaṅkusena sammaggaṃ,",
  "anya-e1002n0:164": "Niyyatyussāhakuñjaro.",
  "anya-e1002n0:165": "Asamekkhitakammantaṃ,",
  "anya-e1002n0:166": "Turitābhi nipātinaṃ;",
  "anya-e1002n0:167": "Tānikammāni tappenti,",
  "anya-e1002n0:168": "Uṇhaṃ va jjhohitaṃ mukhe.",
  "anya-e1002n0:169": "16.",
  "anya-e1002n0:170": " Chaddosā puriseneha,",
  "anya-e1002n0:171": "Hātabbā bhūtimicchantā;",
  "anya-e1002n0:172": "Niddā majjaṃ bhayaṃ kodho,",
  "anya-e1002n0:173": "Ālasyaṃ dīghasuttatā.",
  "anya-e1002n0:174": "Na divā suppasīlena,",
  "anya-e1002n0:175": "Rattimuṭṭhānadessinā;",
  "anya-e1002n0:176": "Niccasoṇḍena mattena,",
  "anya-e1002n0:177": "Sakkā āvasituṃ gharaṃ.",
  "anya-e1002n0:178": "Abhetabbamhi bhāyanti,",
  "anya-e1002n0:179": "Bhāyitabbe nabhāyare;",
  "anya-e1002n0:180": "Bhayābhaya vimuḷhā te,",
  "anya-e1002n0:181": "Jimhānugā ujuñjahā.",
  "anya-e1002n0:182": "Yassa manussabhūtassa,",
  "anya-e1002n0:183": "Natthi bhogāca sippakaṃ;",
  "anya-e1002n0:184": "Kiṃ phalaṃ tassa mānussaṃ,",
  "anya-e1002n0:185": "Dvipādaṭṭho hi so migo.",
  "anya-e1002n0:186": "17.",
  "anya-e1002n0:187": " Nānopāyova kattabbo,",
  "anya-e1002n0:188": "Sace bhaveyya attano;",
  "anya-e1002n0:189": "Atthasiddhi yathākāmaṃ,",
  "anya-e1002n0:190": "Upāyo hi hitañjaso.",
  "anya-e1002n0:191": "Lañjadānabālisena,",
  "anya-e1002n0:192": "Kūṭaḍḍakāradhīvarā;",
  "anya-e1002n0:193": "Vinicchayamahāmacchaṃ,",
  "anya-e1002n0:194": "Oṭṭenti lobhasāgare.",
  "anya-e1002n0:195": "Yassete caturo dhammā,",
  "anya-e1002n0:196": "Vānarinda yathātava;",
  "anya-e1002n0:197": "Saccaṃ dhammo dhīti cāgo,",
  "anya-e1002n0:198": "Diṭṭhaṃ so ativattati.",
  "anya-e1002n0:199": "18.",
  "anya-e1002n0:200": " Vidvāca ratanaṃ nārī,",
  "anya-e1002n0:201": "Vīṇā sātthaṃ giraṃmahī;",
  "anya-e1002n0:202": "Guṇavisesa māgamma,",
  "anya-e1002n0:203": "Guṇāni aguṇānica.",
  "anya-e1002n0:204": "Dhanavā balavā loke,",
  "anya-e1002n0:205": "Dhanā bhavati paṇḍito.",
  "anya-e1002n0:206": "Sumane nissito kīṭo,",
  "anya-e1002n0:207": "Nigguṇo hīnako sayaṃ;",
  "anya-e1002n0:208": "Taṃ pupphehi maṇḍentānaṃ,",
  "anya-e1002n0:209": "Raññaṃ siropi rohati.",
  "anya-e1002n0:210": "Alakkhikehi sañcītā,",
  "anya-e1002n0:211": "Dhanabhogāca cintitā;",
  "anya-e1002n0:212": "Lakkhikassa bhavantete,",
  "anya-e1002n0:213": "Lakkhivā suṭṭhubhuñjati.",
  "anya-e1002n0:214": "Khattiyo seṭṭho jane tasmiṃ,",
  "anya-e1002n0:215": "Yo gottapaṭisārino;",
  "anya-e1002n0:216": "Vijjācaraṇasampanno,",
  "anya-e1002n0:217": "So seṭṭho devamānuse.",
  "anya-e1002n0:218": "Visāpi amataṃ gaṇhe,",
  "anya-e1002n0:219": "Gūthato maṇimuttamaṃ;",
  "anya-e1002n0:220": "Kaṇṭakapādapā pupphaṃ,",
  "anya-e1002n0:221": "Thirataṃ dukkulā varaṃ.",
  "anya-e1002n0:222": "Dhanissarādiguṇommi -",
  "anya-e1002n0:223": "Vegena vāhitā pajā.",
  "anya-e1002n0:224": "19.",
  "anya-e1002n0:225": " Yassa tthi satataṃ mettā,",
  "anya-e1002n0:226": "Sabbalokasuvallabhā;",
  "anya-e1002n0:227": "Kūpāyate samuddopi,",
  "anya-e1002n0:228": "Aggi tassa jalāyate.",
  "anya-e1002n0:229": "20.",
  "anya-e1002n0:230": " Sakkharāyati merūpi,",
  "anya-e1002n0:231": "Visabhakkho sudhāyate;",
  "anya-e1002n0:232": "Sasāyate migarāja,",
  "anya-e1002n0:233": "Byālo mālāguṇāyate;",
  "anya-e1002n0:234": "Dolāyate chamācālo,",
  "anya-e1002n0:235": "Nānāvudhā tiṇāyare.",
  "anya-e1002n0:236": "21.",
  "anya-e1002n0:237": " Sameva sati ussāhe,",
  "anya-e1002n0:238": "Sukhavāho hitaṅkaro;",
  "anya-e1002n0:239": "Ūne-dhike tathā nohi,",
  "anya-e1002n0:240": "Majjhago sādhu sabbadā.",
  "anya-e1002n0:241": "Sādhu kho paṇḍitonāma,",
  "anya-e1002n0:242": "Natveva atipaṇḍito."
}