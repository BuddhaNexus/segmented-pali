{
  "anya-e1004n4:0": "Itthikathā",
  "anya-e1004n4:1": "198.",
  "anya-e1004n4:2": " Namātarā dhītuyā vāpi,",
  "anya-e1004n4:3": "Bhagīniyā vicakkhaṇo;",
  "anya-e1004n4:4": "Navivittāsane mante,",
  "anya-e1004n4:5": "Nārī māyāvinīnanu.",
  "anya-e1004n4:6": "199.",
  "anya-e1004n4:7": " Vijjutānañca lolattaṃ,",
  "anya-e1004n4:8": "Satthānañcatitikkhaṇaṃ;",
  "anya-e1004n4:9": "Sīghataṃ vāyutejānaṃ,",
  "anya-e1004n4:10": "Anukubbanti nāriyo.",
  "anya-e1004n4:11": "200.",
  "anya-e1004n4:12": " Dviguṇo thīna māhāro,",
  "anya-e1004n4:13": "Buddhicāpi catugguṇo;",
  "anya-e1004n4:14": "Chaguṇo hoti vāyāmo,",
  "anya-e1004n4:15": "Kāmotva ṭṭha guṇobhave.",
  "anya-e1004n4:16": "201.",
  "anya-e1004n4:17": " Ekamekāyaitthīyā,",
  "anya-e1004n4:18": "Aṭṭhaṭṭhapatino siyuṃ;",
  "anya-e1004n4:19": "Sūrāca balavantoca,",
  "anya-e1004n4:20": "Sabbākāmarasāharā;",
  "anya-e1004n4:21": "Kareyya navame chandaṃ,",
  "anya-e1004n4:22": "Unnattā hi napūrati.",
  "anya-e1004n4:23": "202.",
  "anya-e1004n4:24": " Lapanti saddhimaññena,",
  "anya-e1004n4:25": "Passantaññaṃ savibbhamā;",
  "anya-e1004n4:26": "Cittakaṃ cintayantaññaṃ,",
  "anya-e1004n4:27": "Nārīnaṃnāma ko piyo.",
  "anya-e1004n4:28": "203.",
  "anya-e1004n4:29": " Jivhāsahassiko yo hi,",
  "anya-e1004n4:30": "Jīve vassasataṃ naro;",
  "anya-e1004n4:31": "Tena nikammuno vutto,",
  "anya-e1004n4:32": "Thīdoso kiṃkhayaṃgato.",
  "anya-e1004n4:33": "204.",
  "anya-e1004n4:34": " Aggi āpo thiyo muḷho,",
  "anya-e1004n4:35": "Sappo rājakulānica;",
  "anya-e1004n4:36": "Payatanāpagantabbo,",
  "anya-e1004n4:37": "Maccupāṇaharāniti.",
  "anya-e1004n4:38": "205.",
  "anya-e1004n4:39": " Itthīnaṃ dujjanā nañca,",
  "anya-e1004n4:40": "Visāsonopapajjate;",
  "anya-e1004n4:41": "Vīsassa siṅgino roga,",
  "anya-e1004n4:42": "Nadīrājakulassaca.",
  "anya-e1004n4:43": "206.",
  "anya-e1004n4:44": " Satthaṃ sunicchitamatīpi vicintanīyaṃ,",
  "anya-e1004n4:45": "Svārādhitopya vanipo parisaṅkanīyo;",
  "anya-e1004n4:46": "Hatthaṅgatāpi yuvatīparirakkhanīyā,",
  "anya-e1004n4:47": "Satthāvanīpayuvatīsu kutovasitthaṃ.",
  "anya-e1004n4:48": "207.",
  "anya-e1004n4:49": " Ayuttakammāraddhanaṃ virodho,",
  "anya-e1004n4:50": "Saṅghassa yuddhañca mahābalehi;",
  "anya-e1004n4:51": "Vissāsakammaṃ pamādāsu niccaṃ,",
  "anya-e1004n4:52": "Dvārānimaccussa vadanti paññā.",
  "anya-e1004n4:53": "208.",
  "anya-e1004n4:54": " Vātaṃ jālena naro parāmase,",
  "anya-e1004n4:55": "Osiñce sāgaraṃ ekapāṇinā;",
  "anya-e1004n4:56": "Sakenatālena janeyya ghosaṃ,",
  "anya-e1004n4:57": "Yo sabbabhāvaṃ pamadāsu osaje.",
  "anya-e1004n4:58": "209.",
  "anya-e1004n4:59": " Itthīpi hi ekacciyā,",
  "anya-e1004n4:60": "Seyyā vuttāva muninā;",
  "anya-e1004n4:61": "Bhaṇḍāna muttamaṃ itthī,",
  "anya-e1004n4:62": "Aggupaṭṭhāyikātica.",
  "anya-e1004n4:63": "210.",
  "anya-e1004n4:64": " Yo naṃ bharati sabbadā,",
  "anya-e1004n4:65": "Niccaṃ ātāpi ussuko;",
  "anya-e1004n4:66": "Sabbakāmaharaṃ posaṃ,",
  "anya-e1004n4:67": "Bhattāraṃ nātimaññati.",
  "anya-e1004n4:68": "211.",
  "anya-e1004n4:69": " Nācāpi sotthi bhattāraṃ,",
  "anya-e1004n4:70": "Issācārena rosaye;",
  "anya-e1004n4:71": "Bhattuca garunosabbe,",
  "anya-e1004n4:72": "Paṭipūjeti paṇḍito.",
  "anya-e1004n4:73": "212.",
  "anya-e1004n4:74": " Uṭṭhāyikā anālāsā,",
  "anya-e1004n4:75": "Saṅgahitaparijjanā;",
  "anya-e1004n4:76": "Bhattu manāpaṃ carati,",
  "anya-e1004n4:77": "Bhattakaṃ anurakkhati.",
  "anya-e1004n4:78": "213.",
  "anya-e1004n4:79": " Evaṃ vattati yānārī,",
  "anya-e1004n4:80": "Bhattuchandavasānugā;",
  "anya-e1004n4:81": "Manāpānāma tedevā,",
  "anya-e1004n4:82": "Yattha sā upapajjati.",
  "anya-e1004n4:83": "214.",
  "anya-e1004n4:84": " Kokilānaṃ saro rūpaṃ,",
  "anya-e1004n4:85": "Nārīrūpaṃ patibbataṃ;",
  "anya-e1004n4:86": "Vijjā rūpa marūpānaṃ,",
  "anya-e1004n4:87": "Khamā rūpaṃ tapassīnaṃ.",
  "anya-e1004n4:88": "215.",
  "anya-e1004n4:89": " Āneya kulajaṃ pañño,",
  "anya-e1004n4:90": "Virūpamapi kaññakaṃ;",
  "anya-e1004n4:91": "Hīnāyapi surūpāya,",
  "anya-e1004n4:92": "Vivāhaṃsadisaṃ kare.",
  "anya-e1004n4:93": "216.",
  "anya-e1004n4:94": " Visamhāmatamādeyya ,",
  "anya-e1004n4:95": "Amajjamhāpikañcanaṃ;",
  "anya-e1004n4:96": "Nicamhāpyuttamāvijjā,",
  "anya-e1004n4:97": "Ratanitthīpidukulā.",
  "anya-e1004n4:98": "217.",
  "anya-e1004n4:99": " Bālitthī makkhikātuṇḍaṃ,",
  "anya-e1004n4:100": "Isīnañca kamaṇḍaluṃ;",
  "anya-e1004n4:101": "Setambu phalaṃ tambulaṃ,",
  "anya-e1004n4:102": "Nocchiṭṭhamupajāyate.",
  "anya-e1004n4:103": "218.",
  "anya-e1004n4:104": " Bālakko paṇṇadhūmoca,",
  "anya-e1004n4:105": "Vuddhitthī pallalodakaṃ;",
  "anya-e1004n4:106": "Āyukkhayakaraṃ niccaṃ,",
  "anya-e1004n4:107": "Rattoca dadhibhojanaṃ.",
  "anya-e1004n4:108": "219.",
  "anya-e1004n4:109": " Thiyo seveyya naccantaṃ,",
  "anya-e1004n4:110": "Sāduṃ bhuñjeyyanāhitaṃ;",
  "anya-e1004n4:111": "Pūjaye mānaye vuddhe,",
  "anya-e1004n4:112": "Guruṃ māyāya nobhaje.",
  "anya-e1004n4:113": "220.",
  "anya-e1004n4:114": " Ācāro kula mākhyāti,",
  "anya-e1004n4:115": "Desa mākhyāti bhāsitaṃ;",
  "anya-e1004n4:116": "Sambhavo pema mākhyāti,",
  "anya-e1004n4:117": "Deha mākhyātibhojanaṃ.",
  "anya-e1004n4:118": "221.",
  "anya-e1004n4:119": " Dehītivacanādvārā ,",
  "anya-e1004n4:120": "Dehaṭṭhāpañcadevatā;",
  "anya-e1004n4:121": "Sajja niyanti dhī kitti,",
  "anya-e1004n4:122": "Hirī sirī matīpica.",
  "anya-e1004n4:123": "222.",
  "anya-e1004n4:124": " Dehītivacanaṃ dukkhaṃ,",
  "anya-e1004n4:125": "Natthītivacanaṃ tathā;",
  "anya-e1004n4:126": "Vākyaṃdehītinatthīti,",
  "anya-e1004n4:127": "Mābhaveyya bhavebhave.",
  "anya-e1004n4:128": "223.",
  "anya-e1004n4:129": " Bodhayanti nayācanti,",
  "anya-e1004n4:130": "Dehīti pacchimājanā;",
  "anya-e1004n4:131": "Passa vatthumadānassa,",
  "anya-e1004n4:132": "Mābhavatūti īdiso.",
  "anya-e1004n4:133": "224.",
  "anya-e1004n4:134": " Mahā atyappakaṃ yāti,",
  "anya-e1004n4:135": "Niguṇe guṇavāpiha;",
  "anya-e1004n4:136": "Aṭṭhānaṭṭheyyabhāvena,",
  "anya-e1004n4:137": "Gajindoiva dabbake.",
  "anya-e1004n4:138": "225.",
  "anya-e1004n4:139": " Mahantaṃ nissayaṃkatvā,",
  "anya-e1004n4:140": "Khuddopyatimahā bhave;",
  "anya-e1004n4:141": "Hemapabbatamāpajja,",
  "anya-e1004n4:142": "Sovaṇṇākira pakkhino.",
  "anya-e1004n4:143": "226.",
  "anya-e1004n4:144": " Bahūna mappasārānaṃ,",
  "anya-e1004n4:145": "Ekibhāvo hidujjayo;",
  "anya-e1004n4:146": "Tiṇena vaṭṭate rajju,",
  "anya-e1004n4:147": "Tāya nāgopi bandhate.",
  "anya-e1004n4:148": "227.",
  "anya-e1004n4:149": " Asahāyo samatthopi,",
  "anya-e1004n4:150": "Tejasi kiṃ karissati;",
  "anya-e1004n4:151": "Nivātasaṇṭhito aggi,",
  "anya-e1004n4:152": "Sayamevūpasammati.",
  "anya-e1004n4:153": "228.",
  "anya-e1004n4:154": " Khantuṃ tapanajotejo,",
  "anya-e1004n4:155": "Sakkāhoti na vaṇṇajo;",
  "anya-e1004n4:156": "Bhūpādīhi katodaṇḍo,",
  "anya-e1004n4:157": "Sakkāhoti na bhaccajo.",
  "anya-e1004n4:158": "229.",
  "anya-e1004n4:159": " Thīsaṃsagge kutosuddho,",
  "anya-e1004n4:160": "Maṃsabhakkhe kutodayā;",
  "anya-e1004n4:161": "Surāpāṇe kutosaccaṃ,",
  "anya-e1004n4:162": "Pakodhamhi kutotapo.",
  "anya-e1004n4:163": "230.",
  "anya-e1004n4:164": " Thīyā guyhaṃ nasaṃseyya,",
  "anya-e1004n4:165": "Amittassaca paṇḍito;",
  "anya-e1004n4:166": "Yocā missena saṃhīro,",
  "anya-e1004n4:167": "Hadayattheno yonaro.",
  "anya-e1004n4:168": "231.",
  "anya-e1004n4:169": " Guyhamattha ",
  "anya-e1004n4:170": "Sambodhayati yonaro;",
  "anya-e1004n4:171": "Mantabhedabhayā tassa,",
  "anya-e1004n4:172": "Dāsabhūto titikkhati.",
  "anya-e1004n4:173": "232.",
  "anya-e1004n4:174": " Vahe amitta khandhena,",
  "anya-e1004n4:175": "Yāvakāle anāgate;",
  "anya-e1004n4:176": "Tamheva cāgate kāle,",
  "anya-e1004n4:177": "Bhinde ghaṭami vuppale.",
  "anya-e1004n4:178": "233.",
  "anya-e1004n4:179": " Khalaṃ sālaṃ pasuṃ khettaṃ,",
  "anya-e1004n4:180": "Gantvācassa abhikkhaṇaṃ;",
  "anya-e1004n4:181": "Mitaṃ dhaññaṃ nidhāpeyya,",
  "anya-e1004n4:182": "Mitañca pācaye ghare.",
  "anya-e1004n4:183": "234.",
  "anya-e1004n4:184": " Kodhaṃ lobhaṃ madaṃ mānaṃ,",
  "anya-e1004n4:185": "Tandiṃ missaṃ pamattakaṃ;",
  "anya-e1004n4:186": "Soṇṭhaṃniddhālukaṃ makkhaṃ,",
  "anya-e1004n4:187": "Maccherañca jahe budho.",
  "anya-e1004n4:188": "235.",
  "anya-e1004n4:189": " Kodho abbhantare jāto,",
  "anya-e1004n4:190": "Dhūvaṃ nāseti kodhanaṃ;",
  "anya-e1004n4:191": "Vatthālaṅkārapuṇṇāyaṃ,",
  "anya-e1004n4:192": "Mañjūsāyaṃ sikhīyathā.",
  "anya-e1004n4:193": "236.",
  "anya-e1004n4:194": " Uppajjate sace kodho;",
  "anya-e1004n4:195": "Āvajje kakacūpamaṃ;",
  "anya-e1004n4:196": "Uppajje ce rase taṇhā,",
  "anya-e1004n4:197": "Puttamaṃsūpamaṃ sare.",
  "anya-e1004n4:198": "237.",
  "anya-e1004n4:199": " Guṇa maddhisamaṃ makkhe,",
  "anya-e1004n4:200": "Parena kalahe sati;",
  "anya-e1004n4:201": "Addhisamaṃ pakāseti,",
  "anya-e1004n4:202": "Aṇumattaṃvavajjakaṃ.",
  "anya-e1004n4:203": "238.",
  "anya-e1004n4:204": " Tasseva tena pāpīyo,",
  "anya-e1004n4:205": "Yo kuddhaṃ paṭikujjhati;",
  "anya-e1004n4:206": "Kuddha mapaṭikujjhanto,",
  "anya-e1004n4:207": "Saṅgāmaṃ jeti dujjayaṃ.",
  "anya-e1004n4:208": "239.",
  "anya-e1004n4:209": " Rāgonāma manosallaṃ,",
  "anya-e1004n4:210": "Guṇatthavaratakkaro;",
  "anya-e1004n4:211": "Rāhu vijjāsasaṅkassa,",
  "anya-e1004n4:212": "Tapodhanahutāsano.",
  "anya-e1004n4:213": "240.",
  "anya-e1004n4:214": " Pamādo jāyate madā,",
  "anya-e1004n4:215": "Pamādā jāyate khayo;",
  "anya-e1004n4:216": "Khayā padosā jāyanti,",
  "anya-e1004n4:217": "Madaṃ kiṃ najahe budho.",
  "anya-e1004n4:218": "241.",
  "anya-e1004n4:219": " Namanti phalinorukkhā,",
  "anya-e1004n4:220": "Namanti vibudhājanā;",
  "anya-e1004n4:221": "Sukkhakaṭṭhañca muḷhoca,",
  "anya-e1004n4:222": "Bhijjateva nanamanti.",
  "anya-e1004n4:223": "242.",
  "anya-e1004n4:224": " Ṭhāne vuddhāna mokāsaṃ,",
  "anya-e1004n4:225": "Dade vuddhāpacāyiko;",
  "anya-e1004n4:226": "Nanu tālo ajīvopi,",
  "anya-e1004n4:227": "Samīpa ññe paronato.",
  "anya-e1004n4:228": "243.",
  "anya-e1004n4:229": " Garukātabbaposesu,",
  "anya-e1004n4:230": "Niccavuttiṃ karoti yo;",
  "anya-e1004n4:231": "Nicattaṃ so pahantāna,",
  "anya-e1004n4:232": "Uttamatte patiṭṭhati.",
  "anya-e1004n4:233": "244.",
  "anya-e1004n4:234": " Yattha posaṃ najānanti,",
  "anya-e1004n4:235": "Jātiyā vinayena vā;",
  "anya-e1004n4:236": "Na tattha mānaṃ kariyā,",
  "anya-e1004n4:237": "Vasa maññātakejane.",
  "anya-e1004n4:238": "245.",
  "anya-e1004n4:239": " Aññātavāsaṃ vasatā,",
  "anya-e1004n4:240": "Jātavedasamenāpi;",
  "anya-e1004n4:241": "Khamitabbaṃ sapaññena,",
  "anya-e1004n4:242": "Api dāsassa tajjitaṃ.",
  "anya-e1004n4:243": "246.",
  "anya-e1004n4:244": " Dhanadhaññāpayogesu,",
  "anya-e1004n4:245": "Tathā vijjāgamesuca;",
  "anya-e1004n4:246": "Dūteca byavahāreca,",
  "anya-e1004n4:247": "Cattalajjo sadā bhave.",
  "anya-e1004n4:248": "247.",
  "anya-e1004n4:249": " Nahi koci kate kicce,",
  "anya-e1004n4:250": "Kattāraṃ sammapekkhate;",
  "anya-e1004n4:251": "Tasmā sabbānikammāni,",
  "anya-e1004n4:252": "Sāvasesāni kāraye.",
  "anya-e1004n4:253": "248.",
  "anya-e1004n4:254": " Iṇaseso aggiseso,",
  "anya-e1004n4:255": "Sattuseso tayoime;",
  "anya-e1004n4:256": "Punappunaṃpi vaḍḍhanti,",
  "anya-e1004n4:257": "Tasmā sesaṃ nakāraye.",
  "anya-e1004n4:258": "249.",
  "anya-e1004n4:259": " Natthi vijjāsamaṃ vittaṃ,",
  "anya-e1004n4:260": "Natthi byādhisamo ripu;",
  "anya-e1004n4:261": "Natthi attasamaṃ pemaṃ,",
  "anya-e1004n4:262": "Natthi kammaparaṃ balaṃ.",
  "anya-e1004n4:263": "250.",
  "anya-e1004n4:264": " Attanā kurute lakkhī,",
  "anya-e1004n4:265": "Alakkhīcāpi attanā;",
  "anya-e1004n4:266": "Nahi lakkhī alakkhīca,",
  "anya-e1004n4:267": "Añño aññassa kurute.",
  "anya-e1004n4:268": "251.",
  "anya-e1004n4:269": " Sayaṃ āyaṃ vayaṃ raññā,",
  "anya-e1004n4:270": "Sayaṃ jaññā katākataṃ;",
  "anya-e1004n4:271": "Attanāva bhavakkheyya,",
  "anya-e1004n4:272": "Katāni akatānica.",
  "anya-e1004n4:273": "252.",
  "anya-e1004n4:274": " Upakāraṃ hiteneva,",
  "anya-e1004n4:275": "Sattunā sattu muddhare;",
  "anya-e1004n4:276": "Pādalaggaṃ karaṭṭhena,",
  "anya-e1004n4:277": "Kaṇṭakena kaṇṭakaṃ.",
  "anya-e1004n4:278": "253.",
  "anya-e1004n4:279": " Name namantassa bhaje bhajantaṃ,",
  "anya-e1004n4:280": "Kiccāni krupassa kareyya kiccaṃ;",
  "anya-e1004n4:281": "Nā natthakāmassa kareyya atthaṃ,",
  "anya-e1004n4:282": "Asambhajantaṃpi nasambhajeyya.",
  "anya-e1004n4:283": "254.",
  "anya-e1004n4:284": " Caje cajantaṃ navataṃ kariyā,",
  "anya-e1004n4:285": "Apetacittena nasambhajeyya;",
  "anya-e1004n4:286": "Dijo dumaṃ khīṇaphala mañatvā,",
  "anya-e1004n4:287": "Aññaṃ sapekkheyya mahāti loko."
}