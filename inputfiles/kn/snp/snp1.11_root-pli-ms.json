{
"snp1.11:1.1": "Sutta Nipāta 1.11",
"snp1.11:2.1": "Vijayasutta",
"snp1.11:3.1": "Caraṃ vā yadi vā tiṭṭhaṃ,",
"snp1.11:3.2": "nisinno uda vā sayaṃ;",
"snp1.11:3.3": "Samiñjeti pasāreti,",
"snp1.11:3.4": "esā kāyassa iñjanā.",
"snp1.11:4.1": "Aṭṭhinahārusaṃyutto, #2",
"snp1.11:4.2": "tacamaṃsāvalepano;",
"snp1.11:4.3": "Chaviyā kāyo paṭicchanno,",
"snp1.11:4.4": "yathābhūtaṃ na dissati.",
"snp1.11:5.1": "Antapūro udarapūro, #3",
"snp1.11:5.2": "yakanapeḷassa vatthino;",
"snp1.11:5.3": "Hadayassa papphāsassa,",
"snp1.11:5.4": "vakkassa pihakassa ca.",
"snp1.11:6.1": "Siṅghāṇikāya kheḷassa, #4",
"snp1.11:6.2": "sedassa ca medassa ca;",
"snp1.11:6.3": "Lohitassa lasikāya,",
"snp1.11:6.4": "pittassa ca vasāya ca.",
"snp1.11:7.1": "Athassa navahi sotehi, #5",
"snp1.11:7.2": "asucī savati sabbadā;",
"snp1.11:7.3": "Akkhimhā akkhigūthako,",
"snp1.11:7.4": "kaṇṇamhā kaṇṇagūthako.",
"snp1.11:8.1": "Siṅghāṇikā ca nāsato, #6",
"snp1.11:8.2": "mukhena vamatekadā;",
"snp1.11:8.3": "Pittaṃ semhañca vamati,",
"snp1.11:8.4": "kāyamhā sedajallikā.",
"snp1.11:9.1": "Athassa susiraṃ sīsaṃ, #7",
"snp1.11:9.2": "matthaluṅgassa pūritaṃ;",
"snp1.11:9.3": "Subhato naṃ maññati bālo,",
"snp1.11:9.4": "avijjāya purakkhato.",
"snp1.11:10.1": "Yadā ca so mato seti, #8",
"snp1.11:10.2": "Uddhumāto vinīlako;",
"snp1.11:10.3": "Apaviddho susānasmiṃ,",
"snp1.11:10.4": "Anapekkhā honti ñātayo.",
"snp1.11:11.1": "Khādanti naṃ suvānā ca, #9",
"snp1.11:11.2": "siṅgālā ca vakā kimī;",
"snp1.11:11.3": "Kākā gijjhā ca khādanti,",
"snp1.11:11.4": "ye caññe santi pāṇino.",
"snp1.11:12.1": "Sutvāna buddhavacanaṃ, #10",
"snp1.11:12.2": "Bhikkhu paññāṇavā idha;",
"snp1.11:12.3": "So kho naṃ parijānāti,",
"snp1.11:12.4": "Yathābhūtañhi passati.",
"snp1.11:13.1": "Yathā idaṃ tathā etaṃ, #11",
"snp1.11:13.2": "Yathā etaṃ tathā idaṃ;",
"snp1.11:13.3": "Ajjhattañca bahiddhā ca,",
"snp1.11:13.4": "Kāye chandaṃ virājaye.",
"snp1.11:14.1": "Chandarāgaviratto so, #12",
"snp1.11:14.2": "Bhikkhu paññāṇavā idha;",
"snp1.11:14.3": "Ajjhagā amataṃ santiṃ,",
"snp1.11:14.4": "Nibbānaṃ padamaccutaṃ.",
"snp1.11:15.1": "Dvipādakoyaṃ asuci, #13",
"snp1.11:15.2": "Duggandho parihīrati;",
"snp1.11:15.3": "Nānākuṇapaparipūro,",
"snp1.11:15.4": "Vissavanto tato tato.",
"snp1.11:16.1": "Etādisena kāyena, #14",
"snp1.11:16.2": "Yo maññe uṇṇametave;",
"snp1.11:16.3": "Paraṃ vā avajāneyya,",
"snp1.11:16.4": "Kimaññatra adassanāti.",
"snp1.11:17.1": "Vijayasuttaṃ ekādasamaṃ."
}