{
"snp1.5:1.1": "Sutta Nipāta 1.5",
"snp1.5:2.1": "Cundasutta",
"snp1.5:3.1": "“Pucchāmi muniṃ pahūtapaññaṃ,",
"snp1.5:3.2": "iti cundo kammāraputto",
"snp1.5:3.3": "Buddhaṃ dhammassāmiṃ vītataṇhaṃ;",
"snp1.5:3.4": "Dvipaduttamaṃ sārathīnaṃ pavaraṃ,",
"snp1.5:3.5": "Kati loke samaṇā tadiṅgha brūhi”.",
"snp1.5:4.1": "“Caturo samaṇā na pañcamatthi, #2",
"snp1.5:4.2": "cundāti bhagavā",
"snp1.5:4.3": "Te te āvikaromi sakkhipuṭṭho;",
"snp1.5:4.4": "Maggajino maggadesako ca,",
"snp1.5:4.5": "Magge jīvati yo ca maggadūsī”.",
"snp1.5:5.1": "“Kaṃ maggajinaṃ vadanti buddhā, #3",
"snp1.5:5.2": "iti cundo kammāraputto",
"snp1.5:5.3": "Maggakkhāyī kathaṃ atulyo hoti;",
"snp1.5:5.4": "Magge jīvati me brūhi puṭṭho,",
"snp1.5:5.5": "Atha me āvikarohi maggadūsiṃ”.",
"snp1.5:6.1": "“Yo tiṇṇakathaṅkatho visallo, #4",
"snp1.5:6.2": "Nibbānābhirato anānugiddho;",
"snp1.5:6.3": "Lokassa sadevakassa netā,",
"snp1.5:6.4": "Tādiṃ maggajinaṃ vadanti buddhā.",
"snp1.5:7.1": "Paramaṃ paramanti yodha ñatvā, #5",
"snp1.5:7.2": "Akkhāti vibhajate idheva dhammaṃ;",
"snp1.5:7.3": "Taṃ kaṅkhachidaṃ muniṃ anejaṃ,",
"snp1.5:7.4": "Dutiyaṃ bhikkhunamāhu maggadesiṃ.",
"snp1.5:8.1": "Yo dhammapade sudesite, #6",
"snp1.5:8.2": "Magge jīvati saññato satīmā;",
"snp1.5:8.3": "Anavajjapadāni sevamāno,",
"snp1.5:8.4": "Tatiyaṃ bhikkhunamāhu maggajīviṃ.",
"snp1.5:9.1": "Chadanaṃ katvāna subbatānaṃ, #7",
"snp1.5:9.2": "Pakkhandī kuladūsako pagabbho;",
"snp1.5:9.3": "Māyāvī asaññato palāpo,",
"snp1.5:9.4": "Patirūpena caraṃ sa maggadūsī.",
"snp1.5:10.1": "Ete ca paṭivijjhi yo gahaṭṭho, #8",
"snp1.5:10.2": "Sutavā ariyasāvako sapañño;",
"snp1.5:10.3": "Sabbe netādisāti ñatvā,",
"snp1.5:10.4": "Iti disvā na hāpeti tassa saddhā;",
"snp1.5:10.5": "Kathaṃ hi duṭṭhena asampaduṭṭhaṃ,",
"snp1.5:10.6": "Suddhaṃ asuddhena samaṃ kareyyā”ti.",
"snp1.5:11.1": "Cundasuttaṃ pañcamaṃ."
}