{
  "snp5.14:0.1": "Sutta Nipāta 5.14 ",
  "snp5.14:0.2": "Udayamāṇavapucchā ",
  "snp5.14:1.1": "“Jhāyiṁ virajamāsīnaṁ, ",
  "snp5.14:1.2": "(iccāyasmā udayo) ",
  "snp5.14:1.3": "Katakiccaṁ anāsavaṁ; ",
  "snp5.14:1.4": "Pāraguṁ sabbadhammānaṁ, ",
  "snp5.14:1.5": "Atthi pañhena āgamaṁ; ",
  "snp5.14:1.6": "Aññāvimokkhaṁ pabrūhi, ",
  "snp5.14:1.7": "Avijjāya pabhedanaṁ”. ",
  "snp5.14:2.1": "“Pahānaṁ kāmacchandānaṁ, ",
  "snp5.14:2.2": "(udayāti bhagavā) ",
  "snp5.14:2.3": "Domanassāna cūbhayaṁ; ",
  "snp5.14:2.4": "Thinassa ca panūdanaṁ, ",
  "snp5.14:2.5": "Kukkuccānaṁ nivāraṇaṁ. ",
  "snp5.14:3.1": "Upekkhāsatisaṁsuddhaṁ, ",
  "snp5.14:3.2": "dhammatakkapurejavaṁ; ",
  "snp5.14:3.3": "Aññāvimokkhaṁ pabrūmi, ",
  "snp5.14:3.4": "avijjāya pabhedanaṁ”. ",
  "snp5.14:4.1": "“Kiṁsu saṁyojano loko, ",
  "snp5.14:4.2": "kiṁsu tassa vicāraṇaṁ; ",
  "snp5.14:4.3": "Kissassa vippahānena, ",
  "snp5.14:4.4": "nibbānaṁ iti vuccati”. ",
  "snp5.14:5.1": "“Nandisaṁyojano loko, ",
  "snp5.14:5.2": "vitakkassa vicāraṇaṁ; ",
  "snp5.14:5.3": "Taṇhāya vippahānena, ",
  "snp5.14:5.4": "nibbānaṁ iti vuccati”. ",
  "snp5.14:6.1": "“Kathaṁ satassa carato, ",
  "snp5.14:6.2": "viññāṇaṁ uparujjhati; ",
  "snp5.14:6.3": "Bhagavantaṁ puṭṭhumāgamma, ",
  "snp5.14:6.4": "taṁ suṇoma vaco tava”. ",
  "snp5.14:7.1": "“Ajjhattañca bahiddhā ca, ",
  "snp5.14:7.2": "Vedanaṁ nābhinandato; ",
  "snp5.14:7.3": "Evaṁ satassa carato, ",
  "snp5.14:7.4": "Viññāṇaṁ uparujjhatī”ti. ",
  "snp5.14:7.5": "Udayamāṇavapucchā terasamā. "
}