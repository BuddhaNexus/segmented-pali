{
"snp5.9:1.1": "Sutta Nipāta 5.9",
"snp5.9:2.1": "Hemakamāṇavapucchā",
"snp5.9:3.1": "“Ye me pubbe viyākaṃsu,",
"snp5.9:3.2": "iccāyasmā hemako",
"snp5.9:3.3": "Huraṃ gotamasāsanā;",
"snp5.9:3.4": "Iccāsi iti bhavissati,",
"snp5.9:3.5": "Sabbaṃ taṃ itihītihaṃ;",
"snp5.9:3.6": "Sabbaṃ taṃ takkavaḍḍhanaṃ,",
"snp5.9:3.7": "Nāhaṃ tattha abhiramiṃ.",
"snp5.9:4.1": "Tvañca me dhammamakkhāhi, #2",
"snp5.9:4.2": "taṇhānigghātanaṃ muni;",
"snp5.9:4.3": "Yaṃ viditvā sato caraṃ,",
"snp5.9:4.4": "tare loke visattikaṃ”.",
"snp5.9:5.1": "“Idha diṭṭhasutamutaviññātesu, #3",
"snp5.9:5.2": "Piyarūpesu hemaka;",
"snp5.9:5.3": "Chandarāgavinodanaṃ,",
"snp5.9:5.4": "Nibbānapadamaccutaṃ.",
"snp5.9:6.1": "Etadaññāya ye satā, #4",
"snp5.9:6.2": "Diṭṭhadhammābhinibbutā;",
"snp5.9:6.3": "Upasantā ca te sadā,",
"snp5.9:6.4": "Tiṇṇā loke visattikan”ti.",
"snp5.9:7.1": "Hemakamāṇavapucchā aṭṭhamā."
}