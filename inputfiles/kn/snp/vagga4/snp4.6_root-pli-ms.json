{
  "snp4.6:0.1": "Sutta Nipāta 4.6 ",
  "snp4.6:0.2": "Jarāsutta ",
  "snp4.6:1.1": "Appaṁ vata jīvitaṁ idaṁ, ",
  "snp4.6:1.2": "Oraṁ vassasatāpi miyyati; ",
  "snp4.6:1.3": "Yo cepi aticca jīvati, ",
  "snp4.6:1.4": "Atha kho so jarasāpi miyyati. ",
  "snp4.6:2.1": "Socanti janā mamāyite, ",
  "snp4.6:2.2": "Na hi santi niccā pariggahā; ",
  "snp4.6:2.3": "Vinābhāvasantamevidaṁ, ",
  "snp4.6:2.4": "Iti disvā nāgāramāvase. ",
  "snp4.6:3.1": "Maraṇenapi taṁ pahīyati, ",
  "snp4.6:3.2": "Yaṁ puriso mamidanti maññati; ",
  "snp4.6:3.3": "Etampi viditvā paṇḍito, ",
  "snp4.6:3.4": "Na mamattāya nametha māmako. ",
  "snp4.6:4.1": "Supinena yathāpi saṅgataṁ, ",
  "snp4.6:4.2": "Paṭibuddho puriso na passati; ",
  "snp4.6:4.3": "Evampi piyāyitaṁ janaṁ, ",
  "snp4.6:4.4": "Petaṁ kālaṅkataṁ na passati. ",
  "snp4.6:5.1": "Diṭṭhāpi sutāpi te janā, ",
  "snp4.6:5.2": "Yesaṁ nāmamidaṁ pavuccati; ",
  "snp4.6:5.3": "Nāmaṁyevāvasissati, ",
  "snp4.6:5.4": "Akkheyyaṁ petassa jantuno. ",
  "snp4.6:6.1": "Sokapparidevamaccharaṁ, ",
  "snp4.6:6.2": "Na jahanti giddhā mamāyite; ",
  "snp4.6:6.3": "Tasmā munayo pariggahaṁ, ",
  "snp4.6:6.4": "Hitvā acariṁsu khemadassino. ",
  "snp4.6:7.1": "Patilīnacarassa bhikkhuno, ",
  "snp4.6:7.2": "Bhajamānassa vivittamāsanaṁ; ",
  "snp4.6:7.3": "Sāmaggiyamāhu tassa taṁ, ",
  "snp4.6:7.4": "Yo attānaṁ bhavane na dassaye. ",
  "snp4.6:8.1": "Sabbattha munī anissito, ",
  "snp4.6:8.2": "Na piyaṁ kubbati nopi appiyaṁ; ",
  "snp4.6:8.3": "Tasmiṁ paridevamaccharaṁ, ",
  "snp4.6:8.4": "Paṇṇe vāri yathā na limpati. ",
  "snp4.6:9.1": "Udabindu yathāpi pokkhare, ",
  "snp4.6:9.2": "Padume vāri yathā na limpati; ",
  "snp4.6:9.3": "Evaṁ muni nopalimpati, ",
  "snp4.6:9.4": "Yadidaṁ diṭṭhasutaṁ mutesu vā. ",
  "snp4.6:10.1": "Dhono na hi tena maññati, ",
  "snp4.6:10.2": "Yadidaṁ diṭṭhasutaṁ mutesu vā; ",
  "snp4.6:10.3": "Nāññena visuddhimicchati, ",
  "snp4.6:10.4": "Na hi so rajjati no virajjatīti. ",
  "snp4.6:10.5": "Jarāsuttaṁ chaṭṭhaṁ. "
}