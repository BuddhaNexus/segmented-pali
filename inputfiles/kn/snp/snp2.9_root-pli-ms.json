{
"snp2.9:1.1": "Sutta Nipāta 2.9",
"snp2.9:2.1": "Kiṃsīlasutta",
"snp2.9:3.1": "“Kiṃsīlo kiṃsamācāro,",
"snp2.9:3.2": "kāni kammāni brūhayaṃ;",
"snp2.9:3.3": "Naro sammā niviṭṭhassa,",
"snp2.9:3.4": "uttamatthañca pāpuṇe”.",
"snp2.9:4.1": "“Vuḍḍhāpacāyī anusūyako siyā, #2",
"snp2.9:4.2": "Kālaññū cassa garūnaṃ dassanāya;",
"snp2.9:4.3": "Dhammiṃ kathaṃ erayitaṃ khaṇaññū,",
"snp2.9:4.4": "Suṇeyya sakkacca subhāsitāni.",
"snp2.9:5.1": "Kālena gacche garūnaṃ sakāsaṃ, #3",
"snp2.9:5.2": "Thambhaṃ niraṃkatvā nivātavutti;",
"snp2.9:5.3": "Atthaṃ dhammaṃ saṃyamaṃ brahmacariyaṃ,",
"snp2.9:5.4": "Anussare ceva samācare ca.",
"snp2.9:6.1": "Dhammārāmo dhammarato, #4",
"snp2.9:6.2": "Dhamme ṭhito dhammavinicchayaññū;",
"snp2.9:6.3": "Nevācare dhammasandosavādaṃ,",
"snp2.9:6.4": "Tacchehi nīyetha subhāsitehi.",
"snp2.9:7.1": "Hassaṃ jappaṃ paridevaṃ padosaṃ, #5",
"snp2.9:7.2": "Māyākataṃ kuhanaṃ giddhi mānaṃ;",
"snp2.9:7.3": "Sārambhaṃ kakkasaṃ kasāvañca mucchaṃ,",
"snp2.9:7.4": "Hitvā care vītamado ṭhitatto.",
"snp2.9:8.1": "Viññātasārāni subhāsitāni, #6",
"snp2.9:8.2": "Sutañca viññātasamādhisāraṃ;",
"snp2.9:8.3": "Na tassa paññā ca sutañca vaḍḍhati,",
"snp2.9:8.4": "Yo sāhaso hoti naro pamatto.",
"snp2.9:9.1": "Dhamme ca ye ariyapavedite ratā, #7",
"snp2.9:9.2": "Anuttarā te vacasā manasā kammunā ca;",
"snp2.9:9.3": "Te santisoraccasamādhisaṇṭhitā,",
"snp2.9:9.4": "Sutassa paññāya ca sāramajjhagū”ti.",
"snp2.9:10.1": "Kiṃsīlasuttaṃ navamaṃ."
}