{
"snp4.5:1.1": "Sutta Nipāta 4.5",
"snp4.5:2.1": "Paramaṭṭhakasutta",
"snp4.5:3.1": "Paramanti diṭṭhīsu paribbasāno,",
"snp4.5:3.2": "Yaduttari kurute jantu loke;",
"snp4.5:3.3": "Hīnāti aññe tato sabbamāha,",
"snp4.5:3.4": "Tasmā vivādāni avītivatto.",
"snp4.5:4.1": "Yadattanī passati ānisaṃsaṃ, #2",
"snp4.5:4.2": "Diṭṭhe sute sīlavate mute vā;",
"snp4.5:4.3": "Tadeva so tattha samuggahāya,",
"snp4.5:4.4": "Nihīnato passati sabbamaññaṃ.",
"snp4.5:5.1": "Taṃ vāpi ganthaṃ kusalā vadanti, #3",
"snp4.5:5.2": "Yaṃ nissito passati hīnamaññaṃ;",
"snp4.5:5.3": "Tasmā hi diṭṭhaṃ va sutaṃ mutaṃ vā,",
"snp4.5:5.4": "Sīlabbataṃ bhikkhu na nissayeyya.",
"snp4.5:6.1": "Diṭṭhimpi lokasmiṃ na kappayeyya, #4",
"snp4.5:6.2": "Ñāṇena vā sīlavatena vāpi;",
"snp4.5:6.3": "Samoti attānamanūpaneyya,",
"snp4.5:6.4": "Hīno na maññetha visesi vāpi.",
"snp4.5:7.1": "Attaṃ pahāya anupādiyāno, #5",
"snp4.5:7.2": "Ñāṇepi so nissayaṃ no karoti;",
"snp4.5:7.3": "Sa ve viyattesu na vaggasārī,",
"snp4.5:7.4": "Diṭṭhimpi so na pacceti kiñci.",
"snp4.5:8.1": "Yassūbhayante paṇidhīdha natthi, #6",
"snp4.5:8.2": "Bhavābhavāya idha vā huraṃ vā;",
"snp4.5:8.3": "Nivesanā tassa na santi keci,",
"snp4.5:8.4": "Dhammesu niccheyya samuggahītaṃ.",
"snp4.5:9.1": "Tassīdha diṭṭhe va sute mute vā, #7",
"snp4.5:9.2": "Pakappitā natthi aṇūpi saññā;",
"snp4.5:9.3": "Taṃ brāhmaṇaṃ diṭṭhimanādiyānaṃ,",
"snp4.5:9.4": "Kenīdha lokasmiṃ vikappayeyya.",
"snp4.5:10.1": "Na kappayanti na purekkharonti, #8",
"snp4.5:10.2": "Dhammāpi tesaṃ na paṭicchitāse;",
"snp4.5:10.3": "Na brāhmaṇo sīlavatena neyyo,",
"snp4.5:10.4": "Pāraṅgato na pacceti tādīti.",
"snp4.5:11.1": "Paramaṭṭhakasuttaṃ pañcamaṃ."
}