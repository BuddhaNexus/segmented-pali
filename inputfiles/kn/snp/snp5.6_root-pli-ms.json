{
"snp5.6:1.1": "Sutta Nipāta 5.6",
"snp5.6:2.1": "Dhotakamāṇavapucchā",
"snp5.6:3.1": "“Pucchāmi taṃ bhagavā brūhi metaṃ,",
"snp5.6:3.2": "iccāyasmā dhotako",
"snp5.6:3.3": "Vācābhikaṅkhāmi mahesi tuyhaṃ;",
"snp5.6:3.4": "Tava sutvāna nigghosaṃ,",
"snp5.6:3.5": "Sikkhe nibbānamattano”.",
"snp5.6:4.1": "“Tenahātappaṃ karohi, #2",
"snp5.6:4.2": "dhotakāti bhagavā",
"snp5.6:4.3": "Idheva nipako sato;",
"snp5.6:4.4": "Ito sutvāna nigghosaṃ,",
"snp5.6:4.5": "Sikkhe nibbānamattano”.",
"snp5.6:5.1": "“Passāmahaṃ devamanussaloke, #3",
"snp5.6:5.2": "Akiñcanaṃ brāhmaṇamiriyamānaṃ;",
"snp5.6:5.3": "Taṃ taṃ namassāmi samantacakkhu,",
"snp5.6:5.4": "Pamuñca maṃ sakka kathaṅkathāhi”.",
"snp5.6:6.1": "“Nāhaṃ sahissāmi pamocanāya, #4",
"snp5.6:6.2": "Kathaṃkathiṃ dhotaka kañci loke;",
"snp5.6:6.3": "Dhammañca seṭṭhaṃ abhijānamāno,",
"snp5.6:6.4": "Evaṃ tuvaṃ oghamimaṃ taresi”.",
"snp5.6:7.1": "“Anusāsa brahme karuṇāyamāno, #5",
"snp5.6:7.2": "Vivekadhammaṃ yamahaṃ vijaññaṃ;",
"snp5.6:7.3": "Yathāhaṃ ākāsova abyāpajjamāno,",
"snp5.6:7.4": "Idheva santo asito careyyaṃ”.",
"snp5.6:8.1": "“Kittayissāmi te santiṃ, #6",
"snp5.6:8.2": "dhotakāti bhagavā",
"snp5.6:8.3": "Diṭṭhe dhamme anītihaṃ;",
"snp5.6:8.4": "Yaṃ viditvā sato caraṃ,",
"snp5.6:8.5": "Tare loke visattikaṃ”.",
"snp5.6:9.1": "“Tañcāhaṃ abhinandāmi, #7",
"snp5.6:9.2": "mahesi santimuttamaṃ;",
"snp5.6:9.3": "Yaṃ viditvā sato caraṃ,",
"snp5.6:9.4": "tare loke visattikaṃ”.",
"snp5.6:10.1": "“Yaṃ kiñci sampajānāsi, #8",
"snp5.6:10.2": "dhotakāti bhagavā",
"snp5.6:10.3": "Uddhaṃ adho tiriyañcāpi majjhe;",
"snp5.6:10.4": "Etaṃ viditvā saṅgoti loke,",
"snp5.6:10.5": "Bhavābhavāya mākāsi taṇhan”ti.",
"snp5.6:11.1": "Dhotakamāṇavapucchā pañcamī."
}