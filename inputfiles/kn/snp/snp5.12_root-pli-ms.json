{
"snp5.12:1.1": "Sutta Nipāta 5.12",
"snp5.12:2.1": "Jatukaṇṇimāṇavapucchā",
"snp5.12:3.1": "“Sutvānahaṃ vīramakāmakāmiṃ,",
"snp5.12:3.2": "iccāyasmā jatukaṇṇi",
"snp5.12:3.3": "Oghātigaṃ puṭṭhumakāmamāgamaṃ;",
"snp5.12:3.4": "Santipadaṃ brūhi sahajanetta,",
"snp5.12:3.5": "Yathātacchaṃ bhagavā brūhi metaṃ.",
"snp5.12:4.1": "Bhagavā hi kāme abhibhuyya iriyati, #2",
"snp5.12:4.2": "Ādiccova pathaviṃ tejī tejasā;",
"snp5.12:4.3": "Parittapaññassa me bhūripañña,",
"snp5.12:4.4": "Ācikkha dhammaṃ yamahaṃ vijaññaṃ;",
"snp5.12:4.5": "Jātijarāya idha vippahānaṃ”.",
"snp5.12:5.1": "“Kāmesu vinaya gedhaṃ, #3",
"snp5.12:5.2": "jatukaṇṇīti bhagavā",
"snp5.12:5.3": "Nekkhammaṃ daṭṭhu khemato;",
"snp5.12:5.4": "Uggahītaṃ nirattaṃ vā,",
"snp5.12:5.5": "Mā te vijjittha kiñcanaṃ.",
"snp5.12:6.1": "Yaṃ pubbe taṃ visosehi, #4",
"snp5.12:6.2": "pacchā te māhu kiñcanaṃ;",
"snp5.12:6.3": "Majjhe ce no gahessasi,",
"snp5.12:6.4": "upasanto carissasi.",
"snp5.12:7.1": "Sabbaso nāmarūpasmiṃ, #5",
"snp5.12:7.2": "Vītagedhassa brāhmaṇa;",
"snp5.12:7.3": "Āsavāssa na vijjanti,",
"snp5.12:7.4": "Yehi maccuvasaṃ vaje”ti.",
"snp5.12:8.1": "Jatukaṇṇimāṇavapucchā ekādasamā."
}