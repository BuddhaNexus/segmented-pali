{
"snp5.14:1.1": "Sutta Nipāta 5.14",
"snp5.14:2.1": "Udayamāṇavapucchā",
"snp5.14:3.1": "“Jhāyiṃ virajamāsīnaṃ,",
"snp5.14:3.2": "iccāyasmā udayo",
"snp5.14:3.3": "Katakiccaṃ anāsavaṃ;",
"snp5.14:3.4": "Pāraguṃ sabbadhammānaṃ,",
"snp5.14:3.5": "Atthi pañhena āgamaṃ;",
"snp5.14:3.6": "Aññāvimokkhaṃ pabrūhi,",
"snp5.14:3.7": "Avijjāya pabhedanaṃ”.",
"snp5.14:4.1": "“Pahānaṃ kāmacchandānaṃ, #2",
"snp5.14:4.2": "udayāti bhagavā",
"snp5.14:4.3": "Domanassāna cūbhayaṃ;",
"snp5.14:4.4": "Thinassa ca panūdanaṃ,",
"snp5.14:4.5": "Kukkuccānaṃ nivāraṇaṃ.",
"snp5.14:5.1": "Upekkhāsatisaṃsuddhaṃ, #3",
"snp5.14:5.2": "dhammatakkapurejavaṃ;",
"snp5.14:5.3": "Aññāvimokkhaṃ pabrūmi,",
"snp5.14:5.4": "avijjāya pabhedanaṃ”.",
"snp5.14:6.1": "“Kiṃsu saṃyojano loko, #4",
"snp5.14:6.2": "kiṃsu tassa vicāraṇaṃ;",
"snp5.14:6.3": "Kissassa vippahānena,",
"snp5.14:6.4": "nibbānaṃ iti vuccati”.",
"snp5.14:7.1": "“Nandisaṃyojano loko, #5",
"snp5.14:7.2": "vitakkassa vicāraṇaṃ;",
"snp5.14:7.3": "Taṇhāya vippahānena,",
"snp5.14:7.4": "nibbānaṃ iti vuccati”.",
"snp5.14:8.1": "“Kathaṃ satassa carato, #6",
"snp5.14:8.2": "viññāṇaṃ uparujjhati;",
"snp5.14:8.3": "Bhagavantaṃ puṭṭhumāgamma,",
"snp5.14:8.4": "taṃ suṇoma vaco tava”.",
"snp5.14:9.1": "“Ajjhattañca bahiddhā ca, #7",
"snp5.14:9.2": "Vedanaṃ nābhinandato;",
"snp5.14:9.3": "Evaṃ satassa carato,",
"snp5.14:9.4": "Viññāṇaṃ uparujjhatī”ti.",
"snp5.14:10.1": "Udayamāṇavapucchā terasamā."
}