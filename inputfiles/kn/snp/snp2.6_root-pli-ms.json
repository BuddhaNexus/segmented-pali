{
"snp2.6:1.1": "Sutta Nipāta 2.6",
"snp2.6:2.1": "Kapilasutta (dhammacariyasutta)",
"snp2.6:3.1": "Dhammacariyaṃ brahmacariyaṃ,",
"snp2.6:3.2": "Etadāhu vasuttamaṃ;",
"snp2.6:3.3": "Pabbajitopi ce hoti,",
"snp2.6:3.4": "Agārā anagāriyaṃ.",
"snp2.6:4.1": "So ce mukharajātiko, #2",
"snp2.6:4.2": "vihesābhirato mago;",
"snp2.6:4.3": "Jīvitaṃ tassa pāpiyo,",
"snp2.6:4.4": "rajaṃ vaḍḍheti attano.",
"snp2.6:5.1": "Kalahābhirato bhikkhu, #3",
"snp2.6:5.2": "Mohadhammena āvuto;",
"snp2.6:5.3": "Akkhātampi na jānāti,",
"snp2.6:5.4": "Dhammaṃ buddhena desitaṃ.",
"snp2.6:6.1": "Vihesaṃ bhāvitattānaṃ, #4",
"snp2.6:6.2": "avijjāya purakkhato;",
"snp2.6:6.3": "Saṅkilesaṃ na jānāti,",
"snp2.6:6.4": "maggaṃ nirayagāminaṃ.",
"snp2.6:7.1": "Vinipātaṃ samāpanno, #5",
"snp2.6:7.2": "gabbhā gabbhaṃ tamā tamaṃ;",
"snp2.6:7.3": "Sa ve tādisako bhikkhu,",
"snp2.6:7.4": "pecca dukkhaṃ nigacchati.",
"snp2.6:8.1": "Gūthakūpo yathā assa, #6",
"snp2.6:8.2": "sampuṇṇo gaṇavassiko;",
"snp2.6:8.3": "Yo ca evarūpo assa,",
"snp2.6:8.4": "dubbisodho hi sāṅgaṇo.",
"snp2.6:9.1": "Yaṃ evarūpaṃ jānātha, #7",
"snp2.6:9.2": "bhikkhavo gehanissitaṃ;",
"snp2.6:9.3": "Pāpicchaṃ pāpasaṅkappaṃ,",
"snp2.6:9.4": "pāpaācāragocaraṃ.",
"snp2.6:10.1": "Sabbe samaggā hutvāna, #8",
"snp2.6:10.2": "abhinibbajjiyātha naṃ;",
"snp2.6:10.3": "Kāraṇḍavaṃ niddhamatha,",
"snp2.6:10.4": "kasambuṃ apakassatha.",
"snp2.6:11.1": "Tato palāpe vāhetha, #9",
"snp2.6:11.2": "Assamaṇe samaṇamānine;",
"snp2.6:11.3": "Niddhamitvāna pāpicche,",
"snp2.6:11.4": "Pāpaācāragocare.",
"snp2.6:12.1": "Suddhā suddhehi saṃvāsaṃ, #10",
"snp2.6:12.2": "Kappayavho patissatā;",
"snp2.6:12.3": "Tato samaggā nipakā,",
"snp2.6:12.4": "Dukkhassantaṃ karissathāti.",
"snp2.6:13.1": "Dhammacariyasuttaṃ chaṭṭhaṃ."
}