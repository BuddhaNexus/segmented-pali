{
"snp5.10:1.1": "Sutta Nipāta 5.10",
"snp5.10:2.1": "Todeyyamāṇavapucchā",
"snp5.10:3.1": "“Yasmiṃ kāmā na vasanti,",
"snp5.10:3.2": "iccāyasmā todeyyo",
"snp5.10:3.3": "Taṇhā yassa na vijjati;",
"snp5.10:3.4": "Kathaṃkathā ca yo tiṇṇo,",
"snp5.10:3.5": "Vimokkho tassa kīdiso”.",
"snp5.10:4.1": "“Yasmiṃ kāmā na vasanti, #2",
"snp5.10:4.2": "todeyyāti bhagavā",
"snp5.10:4.3": "Taṇhā yassa na vijjati;",
"snp5.10:4.4": "Kathaṅkathā ca yo tiṇṇo,",
"snp5.10:4.5": "Vimokkho tassa nāparo”.",
"snp5.10:5.1": "“Nirāsaso so uda āsasāno, #3",
"snp5.10:5.2": "Paññāṇavā so uda paññakappī;",
"snp5.10:5.3": "Muniṃ ahaṃ sakka yathā vijaññaṃ,",
"snp5.10:5.4": "Taṃ me viyācikkha samantacakkhu”.",
"snp5.10:6.1": "“Nirāsaso so na ca āsasāno, #4",
"snp5.10:6.2": "Paññāṇavā so na ca paññakappī;",
"snp5.10:6.3": "Evampi todeyya muniṃ vijāna,",
"snp5.10:6.4": "Akiñcanaṃ kāmabhave asattan”ti.",
"snp5.10:7.1": "Todeyyamāṇavapucchā navamā."
}