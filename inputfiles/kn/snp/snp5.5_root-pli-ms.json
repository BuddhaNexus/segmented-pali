{
"snp5.5:1.1": "Sutta Nipāta 5.5",
"snp5.5:2.1": "Mettagūmāṇavapucchā",
"snp5.5:3.1": "“Pucchāmi taṃ bhagavā brūhi me taṃ,",
"snp5.5:3.2": "iccāyasmā mettagū",
"snp5.5:3.3": "Maññāmi taṃ vedaguṃ bhāvitattaṃ;",
"snp5.5:3.4": "Kuto nu dukkhā samudāgatā ime,",
"snp5.5:3.5": "Ye keci lokasmimanekarūpā”.",
"snp5.5:4.1": "“Dukkhassa ve maṃ pabhavaṃ apucchasi, #2",
"snp5.5:4.2": "mettagūti bhagavā",
"snp5.5:4.3": "Taṃ te pavakkhāmi yathā pajānaṃ;",
"snp5.5:4.4": "Upadhinidānā pabhavanti dukkhā,",
"snp5.5:4.5": "Ye keci lokasmimanekarūpā.",
"snp5.5:5.1": "Yo ve avidvā upadhiṃ karoti, #3",
"snp5.5:5.2": "Punappunaṃ dukkhamupeti mando;",
"snp5.5:5.3": "Tasmā pajānaṃ upadhiṃ na kayirā,",
"snp5.5:5.4": "Dukkhassa jātippabhavānupassī”.",
"snp5.5:6.1": "“Yaṃ taṃ apucchimha akittayī no, #4",
"snp5.5:6.2": "Aññaṃ taṃ pucchāma tadiṅgha brūhi;",
"snp5.5:6.3": "Kathaṃ nu dhīrā vitaranti oghaṃ,",
"snp5.5:6.4": "Jātiṃ jaraṃ sokapariddavañca;",
"snp5.5:6.5": "Taṃ me muni sādhu viyākarohi,",
"snp5.5:6.6": "Tathā hi te vidito esa dhammo”.",
"snp5.5:7.1": "“Kittayissāmi te dhammaṃ, #5",
"snp5.5:7.2": "mettagūti bhagavā",
"snp5.5:7.3": "Diṭṭhe dhamme anītihaṃ;",
"snp5.5:7.4": "Yaṃ viditvā sato caraṃ,",
"snp5.5:7.5": "Tare loke visattikaṃ”.",
"snp5.5:8.1": "“Tañcāhaṃ abhinandāmi, #6",
"snp5.5:8.2": "mahesi dhammamuttamaṃ;",
"snp5.5:8.3": "Yaṃ viditvā sato caraṃ,",
"snp5.5:8.4": "tare loke visattikaṃ”.",
"snp5.5:9.1": "“Yaṃ kiñci sampajānāsi, #7",
"snp5.5:9.2": "mettagūti bhagavā",
"snp5.5:9.3": "Uddhaṃ adho tiriyañcāpi majjhe;",
"snp5.5:9.4": "Etesu nandiñca nivesanañca,",
"snp5.5:9.5": "Panujja viññāṇaṃ bhave na tiṭṭhe.",
"snp5.5:10.1": "Evaṃvihārī sato appamatto, #8",
"snp5.5:10.2": "Bhikkhu caraṃ hitvā mamāyitāni;",
"snp5.5:10.3": "Jātiṃ jaraṃ sokapariddavañca,",
"snp5.5:10.4": "Idheva vidvā pajaheyya dukkhaṃ”.",
"snp5.5:11.1": "“Etābhinandāmi vaco mahesino, #9",
"snp5.5:11.2": "Sukittitaṃ gotamanūpadhīkaṃ;",
"snp5.5:11.3": "Addhā hi bhagavā pahāsi dukkhaṃ,",
"snp5.5:11.4": "Tathā hi te vidito esa dhammo.",
"snp5.5:12.1": "Te cāpi nūnappajaheyyu dukkhaṃ, #10",
"snp5.5:12.2": "Ye tvaṃ muni aṭṭhitaṃ ovadeyya;",
"snp5.5:12.3": "Taṃ taṃ namassāmi samecca nāga,",
"snp5.5:12.4": "Appeva maṃ bhagavā aṭṭhitaṃ ovadeyya”.",
"snp5.5:13.1": "“Yaṃ brāhmaṇaṃ vedagumābhijaññā, #11",
"snp5.5:13.2": "Akiñcanaṃ kāmabhave asattaṃ;",
"snp5.5:13.3": "Addhā hi so oghamimaṃ atāri,",
"snp5.5:13.4": "Tiṇṇo ca pāraṃ akhilo akaṅkho.",
"snp5.5:14.1": "Vidvā ca yo vedagū naro idha, #12",
"snp5.5:14.2": "Bhavābhave saṅgamimaṃ visajja;",
"snp5.5:14.3": "So vītataṇho anīgho nirāso,",
"snp5.5:14.4": "Atāri so jātijaranti brūmī”ti.",
"snp5.5:15.1": "Mettagūmāṇavapucchā catutthī."
}