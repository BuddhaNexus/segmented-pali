{
"snp5.15:1.1": "Sutta Nipāta 5.15",
"snp5.15:2.1": "Posālamāṇavapucchā",
"snp5.15:3.1": "“Yo atītaṃ ādisati,",
"snp5.15:3.2": "iccāyasmā posālo",
"snp5.15:3.3": "Anejo chinnasaṃsayo;",
"snp5.15:3.4": "Pāraguṃ sabbadhammānaṃ,",
"snp5.15:3.5": "Atthi pañhena āgamaṃ.",
"snp5.15:4.1": "Vibhūtarūpasaññissa, #2",
"snp5.15:4.2": "sabbakāyappahāyino;",
"snp5.15:4.3": "Ajjhattañca bahiddhā ca,",
"snp5.15:4.4": "natthi kiñcīti passato;",
"snp5.15:4.5": "Ñāṇaṃ sakkānupucchāmi,",
"snp5.15:4.6": "kathaṃ neyyo tathāvidho”.",
"snp5.15:5.1": "“Viññāṇaṭṭhitiyo sabbā, #3",
"snp5.15:5.2": "posālāti bhagavā",
"snp5.15:5.3": "Abhijānaṃ tathāgato;",
"snp5.15:5.4": "Tiṭṭhantamenaṃ jānāti,",
"snp5.15:5.5": "Vimuttaṃ tapparāyaṇaṃ.",
"snp5.15:6.1": "Ākiñcaññasambhavaṃ ñatvā, #4",
"snp5.15:6.2": "Nandī saṃyojanaṃ iti;",
"snp5.15:6.3": "Evametaṃ abhiññāya,",
"snp5.15:6.4": "Tato tattha vipassati;",
"snp5.15:6.5": "Etaṃ ñāṇaṃ tathaṃ tassa,",
"snp5.15:6.6": "Brāhmaṇassa vusīmato”ti.",
"snp5.15:7.1": "Posālamāṇavapucchā cuddasamā."
}