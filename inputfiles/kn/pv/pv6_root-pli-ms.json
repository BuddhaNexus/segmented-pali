{
  "pv6:0.1": "Petavatthu ",
  "pv6:0.2": "Uragavagga ",
  "pv6:0.3": "6. Pañcaputtakhādapetivatthu ",
  "pv6:1.1": "“Naggā dubbaṇṇarūpāsi, ",
  "pv6:1.2": "duggandhā pūti vāyasi; ",
  "pv6:1.3": "Makkhikāhi parikiṇṇā, ",
  "pv6:1.4": "kā nu tvaṁ idha tiṭṭhasī”ti. ",
  "pv6:2.1": "“Ahaṁ bhadante petīmhi, ",
  "pv6:2.2": "duggatā yamalokikā; ",
  "pv6:2.3": "Pāpakammaṁ karitvāna, ",
  "pv6:2.4": "petalokaṁ ito gatā. ",
  "pv6:3.1": "Kālena pañca puttāni, ",
  "pv6:3.2": "sāyaṁ pañca punāpare; ",
  "pv6:3.3": "Vijāyitvāna khādāmi, ",
  "pv6:3.4": "tepi nā honti me alaṁ. ",
  "pv6:4.1": "Pariḍayhati dhūmāyati, ",
  "pv6:4.2": "khudāya hadayaṁ mama; ",
  "pv6:4.3": "Pānīyaṁ na labhe pātuṁ, ",
  "pv6:4.4": "passa maṁ byasanaṁ gatan”ti. ",
  "pv6:5.1": "“Kiṁ nu kāyena vācāya, ",
  "pv6:5.2": "manasā dukkaṭaṁ kataṁ; ",
  "pv6:5.3": "Kissa kammavipākena, ",
  "pv6:5.4": "puttamaṁsāni khādasī”ti. ",
  "pv6:6.1": "“Sapatī me gabbhinī āsi, ",
  "pv6:6.2": "tassā pāpaṁ acetayiṁ; ",
  "pv6:6.3": "Sāhaṁ paduṭṭhamanasā, ",
  "pv6:6.4": "akariṁ gabbhapātanaṁ. ",
  "pv6:7.1": "Tassā dvemāsiko gabbho, ",
  "pv6:7.2": "lohitaññeva pagghari; ",
  "pv6:7.3": "Tadassā mātā kupitā, ",
  "pv6:7.4": "mayhaṁ ñātī samānayi; ",
  "pv6:7.5": "Sapathañca maṁ kāresi, ",
  "pv6:7.6": "paribhāsāpayī ca maṁ. ",
  "pv6:8.1": "Sāhaṁ ghorañca sapathaṁ, ",
  "pv6:8.2": "musāvādaṁ abhāsisaṁ; ",
  "pv6:8.3": "Puttamaṁsāni khādāmi, ",
  "pv6:8.4": "sace taṁ pakataṁ mayā. ",
  "pv6:9.1": "Tassa kammassa vipākena, ",
  "pv6:9.2": "musāvādassa cūbhayaṁ; ",
  "pv6:9.3": "Puttamaṁsāni khādāmi, ",
  "pv6:9.4": "pubbalohitamakkhitā”ti. ",
  "pv6:10.1": "Pañcaputtakhādapetivatthu chaṭṭhaṁ. "
}