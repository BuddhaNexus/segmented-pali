{
  "pv34:0.1": "Petavatthu ",
  "pv34:0.2": "Cūḷavagga ",
  "pv34:0.3": "9. Kūṭavinicchayikapetavatthu ",
  "pv34:1.1": "“Mālī kiriṭī kāyūrī, ",
  "pv34:1.2": "gattā te candanussadā; ",
  "pv34:1.3": "Pasannamukhavaṇṇosi, ",
  "pv34:1.4": "sūriyavaṇṇova sobhasi. ",
  "pv34:2.1": "Amānusā pārisajjā, ",
  "pv34:2.2": "ye teme paricārakā; ",
  "pv34:2.3": "Dasa kaññāsahassāni, ",
  "pv34:2.4": "yā temā paricārikā; ",
  "pv34:2.5": "Tā kambukāyūradharā, ",
  "pv34:2.6": "kañcanāveḷabhūsitā. ",
  "pv34:3.1": "Mahānubhāvosi tuvaṁ, ",
  "pv34:3.2": "lomahaṁsanarūpavā; ",
  "pv34:3.3": "Piṭṭhimaṁsāni attano, ",
  "pv34:3.4": "sāmaṁ ukkacca khādasi. ",
  "pv34:4.1": "Kiṁ nu kāyena vācāya, ",
  "pv34:4.2": "manasā dukkuṭaṁ kataṁ; ",
  "pv34:4.3": "Kissa kammavipākena, ",
  "pv34:4.4": "piṭṭhimaṁsāni attano; ",
  "pv34:4.5": "Sāmaṁ ukkacca khādasī”ti. ",
  "pv34:5.1": "“Attanohaṁ anatthāya, ",
  "pv34:5.2": "jīvaloke acārisaṁ; ",
  "pv34:5.3": "Pesuññamusāvādena, ",
  "pv34:5.4": "nikativañcanāya ca. ",
  "pv34:6.1": "Tatthāhaṁ parisaṁ gantvā, ",
  "pv34:6.2": "Saccakāle upaṭṭhite; ",
  "pv34:6.3": "Atthaṁ dhammaṁ nirākatvā, ",
  "pv34:6.4": "Adhammamanuvattisaṁ. ",
  "pv34:7.1": "Evaṁ so khādatattānaṁ, ",
  "pv34:7.2": "yo hoti piṭṭhimaṁsiko; ",
  "pv34:7.3": "Yathāhaṁ ajja khādāmi, ",
  "pv34:7.4": "piṭṭhimaṁsāni attano. ",
  "pv34:8.1": "Tayidaṁ tayā nārada sāmaṁ diṭṭhaṁ, ",
  "pv34:8.2": "Anukampakā ye kusalā vadeyyuṁ; ",
  "pv34:8.3": "Mā pesuṇaṁ mā ca musā abhāṇi, ",
  "pv34:8.4": "Mā khosi piṭṭhimaṁsiko tuvan”ti. ",
  "pv34:9.1": "Kūṭavinicchayikapetavatthu navamaṁ. "
}