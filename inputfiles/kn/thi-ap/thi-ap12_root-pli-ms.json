{
"thi-ap12:1.1": "Therīapadāna",
"thi-ap12:2.1": "Ekūposathikavagga",
"thi-ap12:3.1": "2 Saḷalapupphikātherīapadāna",
"thi-ap12:4.1": "“Candabhāgānadītīre,",
"thi-ap12:4.2": "ahosiṃ kinnarī tadā;",
"thi-ap12:4.3": "Addasāhaṃ devadevaṃ,",
"thi-ap12:4.4": "caṅkamantaṃ narāsabhaṃ.",
"thi-ap12:5.1": "Ocinitvāna saḷalaṃ,",
"thi-ap12:5.2": "buddhaseṭṭhassadāsahaṃ;",
"thi-ap12:5.3": "Upasiṅghi mahāvīro,",
"thi-ap12:5.4": "saḷalaṃ devagandhikaṃ.",
"thi-ap12:6.1": "Paṭiggahetvā sambuddho,",
"thi-ap12:6.2": "vipassī lokanāyako;",
"thi-ap12:6.3": "Upasiṅghi mahāvīro,",
"thi-ap12:6.4": "pekkhamānāya me tadā.",
"thi-ap12:7.1": "Añjaliṃ paggahetvāna,",
"thi-ap12:7.2": "vanditvā dvipaduttamaṃ;",
"thi-ap12:7.3": "Sakaṃ cittaṃ pasādetvā,",
"thi-ap12:7.4": "tato pabbatamāruhiṃ.",
"thi-ap12:8.1": "Ekanavutito kappe,",
"thi-ap12:8.2": "yaṃ pupphamadadiṃ tadā;",
"thi-ap12:8.3": "Duggatiṃ nābhijānāmi,",
"thi-ap12:8.4": "buddhapūjāyidaṃ phalaṃ.",
"thi-ap12:9.1": "Kilesā jhāpitā mayhaṃ, … pe …",
"thi-ap12:9.4": "viharāmi anāsavā.",
"thi-ap12:10.1": "Svāgataṃ vata me āsi, … pe …",
"thi-ap12:10.4": "kataṃ buddhassa sāsanaṃ.",
"thi-ap12:11.1": "Paṭisambhidā catasso, … pe …",
"thi-ap12:11.4": "kataṃ buddhassa sāsanaṃ”.",
"thi-ap12:12.1": "Itthaṃ sudaṃ saḷalapupphikā bhikkhunī imā gāthāyo abhāsitthāti.",
"thi-ap12:13.1": "Saḷalapupphikātheriyāpadānaṃ dutiyaṃ."
}