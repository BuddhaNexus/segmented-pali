{
"thi-ap28:1.1": "Therīapadāna",
"thi-ap28:2.1": "Kuṇḍalakesīvagga",
"thi-ap28:3.1": "8. Yasodharātherīapadāna",
"thi-ap28:4.1": "Ekasmiṃ samaye ramme,",
"thi-ap28:4.2": "iddhe rājagahe pure;",
"thi-ap28:4.3": "Pabbhāramhi varekamhi,",
"thi-ap28:4.4": "vasante naranāyake.",
"thi-ap28:5.1": "Vasantiyā tamhi nagare,",
"thi-ap28:5.2": "ramme bhikkhunupassaye;",
"thi-ap28:5.3": "Yasodharābhikkhuniyā,",
"thi-ap28:5.4": "evaṃ āsi vitakkitaṃ.",
"thi-ap28:6.1": "“Suddhodano mahārājā,",
"thi-ap28:6.2": "gotamī ca pajāpatī;",
"thi-ap28:6.3": "Abhiññātā mahātherā,",
"thi-ap28:6.4": "theriyo ca mahiddhikā.",
"thi-ap28:7.1": "Santiṃ gatāva āsuṃ te,",
"thi-ap28:7.2": "dīpaccīva nirāsavā;",
"thi-ap28:7.3": "Lokanāthe dharanteva,",
"thi-ap28:7.4": "ahampi ca sivaṃ padaṃ.",
"thi-ap28:8.1": "Gamissāmī”ti cintetvā,",
"thi-ap28:8.2": "passantī āyumattano;",
"thi-ap28:8.3": "Passitvā āyusaṅkhāraṃ,",
"thi-ap28:8.4": "tadaheva khayaṃ gataṃ.",
"thi-ap28:9.1": "Pattacīvaramādāya,",
"thi-ap28:9.2": "nikkhamitvā sakassamā;",
"thi-ap28:9.3": "Purakkhatā bhikkhunībhi,",
"thi-ap28:9.4": "satehi sahassehi sā.",
"thi-ap28:10.1": "Mahiddhikā mahāpaññā,",
"thi-ap28:10.2": "sambuddhaṃ upasaṅkami;",
"thi-ap28:10.3": "Sambuddhaṃ abhivādetvā,",
"thi-ap28:10.4": "satthuno cakkalakkhaṇe;",
"thi-ap28:10.5": "Nisinnā ekamantamhi,",
"thi-ap28:10.6": "idaṃ vacanamabravi.",
"thi-ap28:11.1": "“Aṭṭhasattativassāhaṃ,",
"thi-ap28:11.2": "pacchimo vattate vayo;",
"thi-ap28:11.3": "Pabbhāramhi anuppattā,",
"thi-ap28:11.4": "ārocemi mahāmuni.",
"thi-ap28:12.1": "Paripakko vayo mayhaṃ,",
"thi-ap28:12.2": "parittaṃ mama jīvitaṃ;",
"thi-ap28:12.3": "Pahāya vo gamissāmi,",
"thi-ap28:12.4": "kataṃ me saraṇamattano.",
"thi-ap28:13.1": "Vayamhi pacchime kāle,",
"thi-ap28:13.2": "maraṇaṃ uparuddhati;",
"thi-ap28:13.3": "Ajjarattiṃ mahāvīra,",
"thi-ap28:13.4": "pāpuṇissāmi nibbutiṃ.",
"thi-ap28:14.1": "Natthi jāti jarā byādhi,",
"thi-ap28:14.2": "maraṇañca mahāmune;",
"thi-ap28:14.3": "Ajarāmaraṇaṃ puraṃ,",
"thi-ap28:14.4": "gamissāmi asaṅkhataṃ.",
"thi-ap28:15.1": "Yāvatā parisā nāma,",
"thi-ap28:15.2": "samupāsanti satthuno;",
"thi-ap28:15.3": "Aparādhamajānantī,",
"thi-ap28:15.4": "khamantaṃ sammukhā mune.",
"thi-ap28:16.1": "Saṃsaritvā ca saṃsāre,",
"thi-ap28:16.2": "khalitañce mamaṃ tayi;",
"thi-ap28:16.3": "Ārocemi mahāvīra,",
"thi-ap28:16.4": "aparādhaṃ khamassu me”.",
"thi-ap28:17.1": "Sutvāna vacanaṃ tassā,",
"thi-ap28:17.2": "munindo idamabravi;",
"thi-ap28:17.3": "“Kimuttaraṃ te vakkhāmi,",
"thi-ap28:17.4": "nibbānāya vajantiyā.",
"thi-ap28:18.1": "Iddhiñcāpi nidassehi,",
"thi-ap28:18.2": "mama sāsanakārike;",
"thi-ap28:18.3": "Parisānañca sabbāsaṃ,",
"thi-ap28:18.4": "kaṅkhaṃ chindassu yāvatā”.",
"thi-ap28:19.1": "Sutvā taṃ munino vācaṃ,",
"thi-ap28:19.2": "bhikkhunī sā yasodharā;",
"thi-ap28:19.3": "Vanditvā munirājaṃ taṃ,",
"thi-ap28:19.4": "idaṃ vacanamabravi.",
"thi-ap28:20.1": "“Yasodharā ahaṃ vīra,",
"thi-ap28:20.2": "agāre te pajāpati;",
"thi-ap28:20.3": "Sākiyamhi kule jātā,",
"thi-ap28:20.4": "itthiaṅge patiṭṭhitā.",
"thi-ap28:21.1": "Thīnaṃ satasahassānaṃ,",
"thi-ap28:21.2": "navutīnaṃ chaduttari;",
"thi-ap28:21.3": "Agāre te ahaṃ vīra,",
"thi-ap28:21.4": "pāmokkhā sabbā issarā.",
"thi-ap28:22.1": "Rūpācāraguṇūpetā,",
"thi-ap28:22.2": "yobbanaṭṭhā piyaṃvadā;",
"thi-ap28:22.3": "Sabbā maṃ apacāyanti,",
"thi-ap28:22.4": "devatā viya mānusā.",
"thi-ap28:23.1": "Kaññāsatasahassapamukhā,",
"thi-ap28:23.2": "Sakyaputtanivesane;",
"thi-ap28:23.3": "Samānasukhadukkhatā,",
"thi-ap28:23.4": "Devatā viya nandane.",
"thi-ap28:24.1": "Kāmadhātumatikkamma,",
"thi-ap28:24.2": "saṇṭhitā rūpadhātuyā;",
"thi-ap28:24.3": "Rūpena sadisā natthi,",
"thi-ap28:24.4": "ṭhapetvā lokanāyakaṃ”.",
"thi-ap28:25.1": "Sambuddhaṃ abhivādetvā,",
"thi-ap28:25.2": "iddhiṃ dassesi satthuno;",
"thi-ap28:25.3": "Nekā nānāvidhākārā,",
"thi-ap28:25.4": "mahāiddhīpi dassayī.",
"thi-ap28:26.1": "Cakkavāḷasamaṃ kāyaṃ,",
"thi-ap28:26.2": "sīsaṃ uttarato kuru;",
"thi-ap28:26.3": "Ubho pakkhā duve dīpā,",
"thi-ap28:26.4": "jambudīpaṃ sarīrato.",
"thi-ap28:27.1": "Dakkhiṇañca saraṃ piñchaṃ,",
"thi-ap28:27.2": "nānāsākhā tu pattakā;",
"thi-ap28:27.3": "Candañca sūriyañcakkhi,",
"thi-ap28:27.4": "merupabbatato sikhaṃ.",
"thi-ap28:28.1": "Cakkavāḷagiriṃ tuṇḍaṃ,",
"thi-ap28:28.2": "jamburukkhaṃ samūlakaṃ;",
"thi-ap28:28.3": "Bījamānā upāgantvā,",
"thi-ap28:28.4": "vandantī lokanāyakaṃ.",
"thi-ap28:29.1": "Hatthivaṇṇaṃ tathevassaṃ,",
"thi-ap28:29.2": "pabbataṃ jaladhiṃ tathā;",
"thi-ap28:29.3": "Candimaṃ sūriyaṃ meruṃ,",
"thi-ap28:29.4": "sakkavaṇṇañca dassayi.",
"thi-ap28:30.1": "“Yasodharā ahaṃ vīra,",
"thi-ap28:30.2": "pāde vandāmi cakkhuma”;",
"thi-ap28:30.3": "Sahassalokadhātūnaṃ,",
"thi-ap28:30.4": "phullapadmena chādayi.",
"thi-ap28:31.1": "Brahmavaṇṇañca māpetvā,",
"thi-ap28:31.2": "dhammaṃ desesi suññataṃ;",
"thi-ap28:31.3": "“Yasodharā ahaṃ vīra,",
"thi-ap28:31.4": "pāde vandāmi cakkhuma.",
"thi-ap28:32.1": "Iddhīsu ca vasī homi,",
"thi-ap28:32.2": "dibbāya sotadhātuyā;",
"thi-ap28:32.3": "Cetopariyañāṇassa,",
"thi-ap28:32.4": "vasī homi mahāmuni.",
"thi-ap28:33.1": "Pubbenivāsaṃ jānāmi,",
"thi-ap28:33.2": "dibbacakkhu visodhitaṃ;",
"thi-ap28:33.3": "Sabbāsavā parikkhīṇā,",
"thi-ap28:33.4": "natthi dāni punabbhavo.",
"thi-ap28:34.1": "Atthadhammaniruttīsu,",
"thi-ap28:34.2": "paṭibhāne tatheva ca;",
"thi-ap28:34.3": "Ñāṇaṃ mayhaṃ mahāvīra,",
"thi-ap28:34.4": "uppannaṃ tava santike.",
"thi-ap28:35.1": "Pubbānaṃ lokanāthānaṃ,",
"thi-ap28:35.2": "saṅgamaṃ te nidassitaṃ;",
"thi-ap28:35.3": "Adhikāraṃ bahuṃ mayhaṃ,",
"thi-ap28:35.4": "tuyhatthāya mahāmune.",
"thi-ap28:36.1": "Yaṃ mayhaṃ pūritaṃ kammaṃ,",
"thi-ap28:36.2": "Kusalaṃ sarase mune;",
"thi-ap28:36.3": "Tuyhatthāya mahāvīra,",
"thi-ap28:36.4": "Puññaṃ upacitaṃ mayā.",
"thi-ap28:37.1": "Abhabbaṭṭhāne vajjetvā,",
"thi-ap28:37.2": "vārayitvā anācaraṃ;",
"thi-ap28:37.3": "Tuyhatthāya mahāvīra,",
"thi-ap28:37.4": "sañcattaṃ jīvitaṃ mayā.",
"thi-ap28:38.1": "Nekakoṭisahassāni,",
"thi-ap28:38.2": "bhariyatthāyadāsi maṃ;",
"thi-ap28:38.3": "Na tattha vimanā homi,",
"thi-ap28:38.4": "tuyhatthāya mahāmuni.",
"thi-ap28:39.1": "Nekakoṭisahassāni,",
"thi-ap28:39.2": "upakārāyadāsi maṃ;",
"thi-ap28:39.3": "Na tattha vimanā homi,",
"thi-ap28:39.4": "tuyhatthāya mahāmuni.",
"thi-ap28:40.1": "Nekakoṭisahassāni,",
"thi-ap28:40.2": "bhojanatthāyadāsi maṃ;",
"thi-ap28:40.3": "Na tattha vimanā homi,",
"thi-ap28:40.4": "tuyhatthāya mahāmuni.",
"thi-ap28:41.1": "Nekakoṭisahassāni,",
"thi-ap28:41.2": "jīvitāni pariccajiṃ;",
"thi-ap28:41.3": "Bhayamokkhaṃ karissanti,",
"thi-ap28:41.4": "dadāmi mama jīvitaṃ.",
"thi-ap28:42.1": "Aṅgagate alaṅkāre,",
"thi-ap28:42.2": "vatthe nānāvidhe bahū;",
"thi-ap28:42.3": "Itthimaṇḍe na gūhāmi,",
"thi-ap28:42.4": "tuyhatthāya mahāmuni.",
"thi-ap28:43.1": "Dhanadhaññapariccāgaṃ,",
"thi-ap28:43.2": "gāmāni nigamāni ca;",
"thi-ap28:43.3": "Khettaṃ puttā ca dhītā ca,",
"thi-ap28:43.4": "pariccattā mahāmuni.",
"thi-ap28:44.1": "Hatthī assā gavā cāpi,",
"thi-ap28:44.2": "dāsiyo paricārikā;",
"thi-ap28:44.3": "Tuyhatthāya mahāvīra,",
"thi-ap28:44.4": "pariccattā asaṅkhiyā.",
"thi-ap28:45.1": "Yaṃ mayhaṃ paṭimantesi,",
"thi-ap28:45.2": "dānaṃ dassāmi yācake;",
"thi-ap28:45.3": "Vimanaṃ me na passāmi,",
"thi-ap28:45.4": "dadato dānamuttamaṃ.",
"thi-ap28:46.1": "Nānāvidhaṃ bahuṃ dukkhaṃ,",
"thi-ap28:46.2": "saṃsāre ca bahubbidhe;",
"thi-ap28:46.3": "Tuyhatthāya mahāvīra,",
"thi-ap28:46.4": "anubhuttaṃ asaṅkhiyaṃ.",
"thi-ap28:47.1": "Sukhappattānumodāmi,",
"thi-ap28:47.2": "na ca dukkhesu dummanā;",
"thi-ap28:47.3": "Sabbattha tulitā homi,",
"thi-ap28:47.4": "tuyhatthāya mahāmuni.",
"thi-ap28:48.1": "Anumaggena sambuddho,",
"thi-ap28:48.2": "yaṃ dhammaṃ abhinīhari;",
"thi-ap28:48.3": "Anubhotvā sukhaṃ dukkhaṃ,",
"thi-ap28:48.4": "patto bodhiṃ mahāmuni.",
"thi-ap28:49.1": "Brahmadevañca sambuddhaṃ,",
"thi-ap28:49.2": "gotamaṃ lokanāyakaṃ;",
"thi-ap28:49.3": "Aññesaṃ lokanāthānaṃ,",
"thi-ap28:49.4": "saṅgamaṃ te bahuṃ mayā.",
"thi-ap28:50.1": "Adhikāraṃ bahuṃ mayhaṃ,",
"thi-ap28:50.2": "tuyhatthāya mahāmuni;",
"thi-ap28:50.3": "Gavesato buddhadhamme,",
"thi-ap28:50.4": "ahaṃ te paricārikā.",
"thi-ap28:51.1": "Kappe ca satasahasse,",
"thi-ap28:51.2": "caturo ca asaṅkhiye;",
"thi-ap28:51.3": "Dīpaṅkaro mahāvīro,",
"thi-ap28:51.4": "uppajji lokanāyako.",
"thi-ap28:52.1": "Paccantadesavisaye,",
"thi-ap28:52.2": "nimantetvā tathāgataṃ;",
"thi-ap28:52.3": "Tassa āgamanaṃ maggaṃ,",
"thi-ap28:52.4": "sodhenti tuṭṭhamānasā.",
"thi-ap28:53.1": "Tena kālena so āsi,",
"thi-ap28:53.2": "sumedho nāma brāhmaṇo;",
"thi-ap28:53.3": "Maggañca paṭiyādesi,",
"thi-ap28:53.4": "āyato sabbadassino.",
"thi-ap28:54.1": "Tena kālenahaṃ āsiṃ,",
"thi-ap28:54.2": "kaññā brāhmaṇasambhavā;",
"thi-ap28:54.3": "Sumittā nāma nāmena,",
"thi-ap28:54.4": "upagacchiṃ samāgamaṃ.",
"thi-ap28:55.1": "Aṭṭha uppalahatthāni,",
"thi-ap28:55.2": "pūjanatthāya satthuno;",
"thi-ap28:55.3": "Ādāya janasammajjhe,",
"thi-ap28:55.4": "addasaṃ isimuggataṃ.",
"thi-ap28:56.1": "Cirānugataṃ dayitaṃ,",
"thi-ap28:56.2": "atikkantaṃ manoharaṃ;",
"thi-ap28:56.3": "Disvā tadā amaññissaṃ,",
"thi-ap28:56.4": "saphalaṃ jīvitaṃ mama.",
"thi-ap28:57.1": "Parakkamaṃ taṃ saphalaṃ,",
"thi-ap28:57.2": "addasaṃ isino tadā;",
"thi-ap28:57.3": "Pubbakammena sambuddhe,",
"thi-ap28:57.4": "cittañcāpi pasīdi me.",
"thi-ap28:58.1": "Bhiyyo cittaṃ pasādesiṃ,",
"thi-ap28:58.2": "ise uggatamānase;",
"thi-ap28:58.3": "Deyyaṃ aññaṃ na passāmi,",
"thi-ap28:58.4": "demi pupphāni te isi.",
"thi-ap28:59.1": "‘Pañca hatthā tava hontu,",
"thi-ap28:59.2": "tayo hontu mamaṃ ise;",
"thi-ap28:59.3": "Tena saddhiṃ samā hontu,",
"thi-ap28:59.4": "bodhatthāya tavaṃ ise’.",
"thi-ap28:60.1": "Catutthaṃ bhāṇavāraṃ.",
"thi-ap28:61.1": "Isi gahetvā pupphāni,",
"thi-ap28:61.2": "āgacchantaṃ mahāyasaṃ;",
"thi-ap28:61.3": "Pūjesi janasammajjhe,",
"thi-ap28:61.4": "bodhatthāya mahāisi.",
"thi-ap28:62.1": "Passitvā janasammajjhe,",
"thi-ap28:62.2": "dīpaṅkaro mahāmuni;",
"thi-ap28:62.3": "Viyākāsi mahāvīro,",
"thi-ap28:62.4": "isi muggatamānasaṃ.",
"thi-ap28:63.1": "Aparimeyye ito kappe,",
"thi-ap28:63.2": "dīpaṅkaro mahāmuni;",
"thi-ap28:63.3": "Mama kammaṃ viyākāsi,",
"thi-ap28:63.4": "ujubhāvaṃ mahāmuni.",
"thi-ap28:64.1": "‘Samacittā samakammā,",
"thi-ap28:64.2": "samakārī bhavissati;",
"thi-ap28:64.3": "Piyā hessati kammena,",
"thi-ap28:64.4": "tuyhatthāya mahāisi.",
"thi-ap28:65.1": "Sudassanā supiyā ca,",
"thi-ap28:65.2": "manāpā piyavādinī;",
"thi-ap28:65.3": "Tassa dhammesu dāyādā,",
"thi-ap28:65.4": "viharissati iddhikā.",
"thi-ap28:66.1": "Yathāpi bhaṇḍasāmuggaṃ,",
"thi-ap28:66.2": "anurakkhati sāmino;",
"thi-ap28:66.3": "Evaṃ kusaladhammānaṃ,",
"thi-ap28:66.4": "anurakkhissate ayaṃ.",
"thi-ap28:67.1": "Tassa te anukampantī,",
"thi-ap28:67.2": "pūrayissati pāramī;",
"thi-ap28:67.3": "Sīhova pañjaraṃ bhetvā,",
"thi-ap28:67.4": "pāpuṇissati bodhiyaṃ’.",
"thi-ap28:68.1": "Aparimeyye ito kappe,",
"thi-ap28:68.2": "yaṃ maṃ buddho viyākarī;",
"thi-ap28:68.3": "Taṃ vācaṃ anumodentī,",
"thi-ap28:68.4": "evaṃkārī bhaviṃ ahaṃ.",
"thi-ap28:69.1": "Tassa kammassa sukatassa,",
"thi-ap28:69.2": "tattha cittaṃ pasādayiṃ;",
"thi-ap28:69.3": "Devamanussakaṃ yoniṃ,",
"thi-ap28:69.4": "anubhotvā asaṅkhiyaṃ.",
"thi-ap28:70.1": "Sukhadukkhenubhotvāhaṃ,",
"thi-ap28:70.2": "devesu mānusesu ca;",
"thi-ap28:70.3": "Pacchime bhave sampatte,",
"thi-ap28:70.4": "ajāyiṃ sākiye kule.",
"thi-ap28:71.1": "Rūpavatī bhogavatī,",
"thi-ap28:71.2": "yasasīlavatī tato;",
"thi-ap28:71.3": "Sabbaṅgasampadā homi,",
"thi-ap28:71.4": "kulesu abhisakkatā.",
"thi-ap28:72.1": "Lābhaṃ silokaṃ sakkāraṃ,",
"thi-ap28:72.2": "lokadhammasamāgamaṃ;",
"thi-ap28:72.3": "Cittañca dukkhitaṃ natthi,",
"thi-ap28:72.4": "vasāmi akutobhayā.",
"thi-ap28:73.1": "Vuttañhetaṃ bhagavatā,",
"thi-ap28:73.2": "‘rañño antepure tadā;",
"thi-ap28:73.3": "Khattiyānaṃ pure vīra,",
"thi-ap28:73.4": "upakārañca niddisi.",
"thi-ap28:74.1": "Upakārā ca yā nārī,",
"thi-ap28:74.2": "yā ca nārī sukhe dukhe;",
"thi-ap28:74.3": "Atthakkhāyī ca yā nārī,",
"thi-ap28:74.4": "yā ca nārīnukampikā’.",
"thi-ap28:75.1": "Pañcakoṭisatā buddhā,",
"thi-ap28:75.2": "navakoṭisatāni ca;",
"thi-ap28:75.3": "Etesaṃ devadevānaṃ,",
"thi-ap28:75.4": "mahādānaṃ pavattayiṃ.",
"thi-ap28:76.1": "Adhikāraṃ mahā mayhaṃ,",
"thi-ap28:76.2": "dhammarāja suṇohi me;",
"thi-ap28:76.3": "Ekādasakoṭisatā,",
"thi-ap28:76.4": "buddhā dvādasa koṭiyo.",
"thi-ap28:77.1": "Etesaṃ devadevānaṃ,",
"thi-ap28:77.2": "mahādānaṃ pavattayiṃ;",
"thi-ap28:77.3": "Adhikāraṃ mahā mayhaṃ,",
"thi-ap28:77.4": "dhammarāja suṇohi me.",
"thi-ap28:78.1": "Vīsakoṭisatā buddhā,",
"thi-ap28:78.2": "tiṃsakoṭisatāni ca;",
"thi-ap28:78.3": "Etesaṃ devadevānaṃ,",
"thi-ap28:78.4": "mahādānaṃ pavattayiṃ.",
"thi-ap28:79.1": "Adhikāraṃ mahā mayhaṃ,",
"thi-ap28:79.2": "dhammarāja suṇohi me;",
"thi-ap28:79.3": "Cattālīsakoṭisatā,",
"thi-ap28:79.4": "paññāsa koṭisatāni ca.",
"thi-ap28:80.1": "Etesaṃ devadevānaṃ,",
"thi-ap28:80.2": "mahādānaṃ pavattayiṃ;",
"thi-ap28:80.3": "Adhikāraṃ mahā mayhaṃ,",
"thi-ap28:80.4": "dhammarāja suṇohi me.",
"thi-ap28:81.1": "Saṭṭhikoṭisatā buddhā,",
"thi-ap28:81.2": "sattatikoṭisatāni ca;",
"thi-ap28:81.3": "Etesaṃ devadevānaṃ,",
"thi-ap28:81.4": "mahādānaṃ pavattayiṃ.",
"thi-ap28:82.1": "Adhikāraṃ mahā mayhaṃ,",
"thi-ap28:82.2": "dhammarāja suṇohi me;",
"thi-ap28:82.3": "Asītikoṭisatā buddhā,",
"thi-ap28:82.4": "navutikoṭisatāni ca.",
"thi-ap28:83.1": "Etesaṃ devadevānaṃ,",
"thi-ap28:83.2": "mahādānaṃ pavattayiṃ;",
"thi-ap28:83.3": "Adhikāraṃ mahā mayhaṃ,",
"thi-ap28:83.4": "dhammarāja suṇohi me.",
"thi-ap28:84.1": "Koṭisatasahassāni,",
"thi-ap28:84.2": "honti lokagganāyakā;",
"thi-ap28:84.3": "Etesaṃ devadevānaṃ,",
"thi-ap28:84.4": "mahādānaṃ pavattayiṃ.",
"thi-ap28:85.1": "Adhikāraṃ mahā mayhaṃ,",
"thi-ap28:85.2": "dhammarāja suṇohi me;",
"thi-ap28:85.3": "Navakoṭisahassāni,",
"thi-ap28:85.4": "apare lokanāyakā.",
"thi-ap28:86.1": "Etesaṃ devadevānaṃ,",
"thi-ap28:86.2": "mahādānaṃ pavattayiṃ;",
"thi-ap28:86.3": "Adhikāraṃ mahā mayhaṃ,",
"thi-ap28:86.4": "dhammarāja suṇohi me.",
"thi-ap28:87.1": "Koṭisatasahassāni,",
"thi-ap28:87.2": "pañcāsītimahesinaṃ;",
"thi-ap28:87.3": "Pañcāsītikoṭisatā,",
"thi-ap28:87.4": "sattatiṃsā ca koṭiyo.",
"thi-ap28:88.1": "Etesaṃ devadevānaṃ,",
"thi-ap28:88.2": "mahādānaṃ pavattayiṃ;",
"thi-ap28:88.3": "Adhikāraṃ mahā mayhaṃ,",
"thi-ap28:88.4": "dhammarāja suṇohi me.",
"thi-ap28:89.1": "Paccekabuddhā vītarāgā,",
"thi-ap28:89.2": "aṭṭhaṭṭhamakakoṭiyo;",
"thi-ap28:89.3": "Adhikāraṃ mahā mayhaṃ,",
"thi-ap28:89.4": "dhammarāja suṇohi me.",
"thi-ap28:90.1": "Khīṇāsavā vītamalā,",
"thi-ap28:90.2": "asaṅkhiyā buddhasāvakā;",
"thi-ap28:90.3": "Adhikāraṃ mahā mayhaṃ,",
"thi-ap28:90.4": "dhammarāja suṇohi me.",
"thi-ap28:91.1": "Evaṃ dhamme suciṇṇānaṃ,",
"thi-ap28:91.2": "sadā dhammassa cārinaṃ;",
"thi-ap28:91.3": "Dhammacārī sukhaṃ seti,",
"thi-ap28:91.4": "asmiṃ loke paramhi ca.",
"thi-ap28:92.1": "Dhammaṃ care sucaritaṃ,",
"thi-ap28:92.2": "na naṃ duccaritaṃ care;",
"thi-ap28:92.3": "Dhammacārī sukhaṃ seti,",
"thi-ap28:92.4": "asmiṃ loke paramhi ca.",
"thi-ap28:93.1": "Nibbinditvāna saṃsāre,",
"thi-ap28:93.2": "pabbajiṃ anagāriyaṃ;",
"thi-ap28:93.3": "Sahassaparivārena,",
"thi-ap28:93.4": "pabbajitvā akiñcanā.",
"thi-ap28:94.1": "Agāraṃ vijahitvāna,",
"thi-ap28:94.2": "pabbajiṃ anagāriyaṃ;",
"thi-ap28:94.3": "Aḍḍhamāse asampatte,",
"thi-ap28:94.4": "catusaccamapāpuṇiṃ.",
"thi-ap28:95.1": "Cīvaraṃ piṇḍapātañca,",
"thi-ap28:95.2": "paccayaṃ sayanāsanaṃ;",
"thi-ap28:95.3": "Upanenti bahū janā,",
"thi-ap28:95.4": "sāgareyeva ūmiyo.",
"thi-ap28:96.1": "Kilesā jhāpitā mayhaṃ, … pe …",
"thi-ap28:96.4": "viharāmi anāsavā.",
"thi-ap28:97.1": "Svāgataṃ vata me āsi, … pe …",
"thi-ap28:97.4": "kataṃ buddhassa sāsanaṃ.",
"thi-ap28:98.1": "Paṭisambhidā catasso, … pe …",
"thi-ap28:98.4": "kataṃ buddhassa sāsanaṃ.",
"thi-ap28:99.1": "Evaṃ bahuvidhaṃ dukkhaṃ,",
"thi-ap28:99.2": "sampattī ca bahubbidhā;",
"thi-ap28:99.3": "Visuddhibhāvaṃ sampattā,",
"thi-ap28:99.4": "labhāmi sabbasampadā.",
"thi-ap28:100.1": "Yā dadāti sakattānaṃ,",
"thi-ap28:100.2": "puññatthāya mahesino;",
"thi-ap28:100.3": "Sahāyasampadā honti,",
"thi-ap28:100.4": "nibbānapadamasaṅkhataṃ.",
"thi-ap28:101.1": "Parikkhīṇaṃ atītañca,",
"thi-ap28:101.2": "Paccuppannaṃ anāgataṃ;",
"thi-ap28:101.3": "Sabbakammaṃ mamaṃ khīṇaṃ,",
"thi-ap28:101.4": "Pāde vandāmi cakkhuma”.",
"thi-ap28:102.1": "Itthaṃ sudaṃ yasodharā bhikkhunī bhagavato sammukhā imā gāthāyo abhāsitthāti.",
"thi-ap28:103.1": "Yasodharātheriyāpadānaṃ aṭṭhamaṃ."
}