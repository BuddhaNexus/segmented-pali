{
"thi-ap4:1.1": "Therīapadāna",
"thi-ap4:2.1": "Sumedhāvagga",
"thi-ap4:3.1": "4 Saṅkamanatthātherīapadāna",
"thi-ap4:4.1": "“Vipassissa bhagavato,",
"thi-ap4:4.2": "lokajeṭṭhassa tādino;",
"thi-ap4:4.3": "Rathiyaṃ paṭipannassa,",
"thi-ap4:4.4": "tārayantassa pāṇino.",
"thi-ap4:5.1": "Gharato nikkhamitvāna,",
"thi-ap4:5.2": "avakujjā nipajjahaṃ;",
"thi-ap4:5.3": "Anukampako lokanātho,",
"thi-ap4:5.4": "sirasi akkamī mama.",
"thi-ap4:6.1": "Akkamitvāna sirasi,",
"thi-ap4:6.2": "agamā lokanāyako;",
"thi-ap4:6.3": "Tena cittappasādena,",
"thi-ap4:6.4": "tusitaṃ agamāsahaṃ.",
"thi-ap4:7.1": "Kilesā jhāpitā mayhaṃ, … pe …",
"thi-ap4:7.4": "viharāmi anāsavā.",
"thi-ap4:8.1": "Svāgataṃ vata me āsi, … pe …",
"thi-ap4:8.4": "kataṃ buddhassa sāsanaṃ.",
"thi-ap4:9.1": "Paṭisambhidā catasso, … pe …",
"thi-ap4:9.4": "kataṃ buddhassa sāsanaṃ”.",
"thi-ap4:10.1": "Itthaṃ sudaṃ saṅkamanatthā bhikkhunī imā gāthāyo abhāsitthāti.",
"thi-ap4:11.1": "Saṅkamanatthātheriyāpadānaṃ catutthaṃ."
}