{
"iti66:1.1": "Itivuttaka 66",
"iti66:2.1": "Soceyyasutta",
"iti66:3.1": "Vuttañhetaṃ bhagavatā vuttamarahatāti me sutaṃ:",
"iti66:4.1": "“Tīṇimāni, bhikkhave, soceyyāni.",
"iti66:4.2": "Katamāni tīṇi?",
"iti66:4.3": "Kāyasoceyyaṃ, vacīsoceyyaṃ, manosoceyyaṃ—",
"iti66:4.4": "imāni kho, bhikkhave, tīṇi soceyyānī”ti.",
"iti66:4.5": "Etamatthaṃ bhagavā avoca.",
"iti66:4.6": "Tatthetaṃ iti vuccati:",
"iti66:5.1": "“Kāyasuciṃ vacīsuciṃ,",
"iti66:5.2": "cetosucimanāsavaṃ;",
"iti66:5.3": "Suciṃ soceyyasampannaṃ,",
"iti66:5.4": "āhu sabbappahāyinan”ti.",
"iti66:6.1": "Ayampi attho vutto bhagavatā, iti me sutanti.",
"iti66:7.1": "Sattamaṃ."
}