{
"iti73:1.1": "Itivuttaka 73",
"iti73:2.1": "Santatarasutta",
"iti73:3.1": "Vuttañhetaṃ bhagavatā vuttamarahatāti me sutaṃ:",
"iti73:4.1": "“Rūpehi, bhikkhave, arūpā santatarā, arūpehi nirodho santataro”ti.",
"iti73:4.2": "Etamatthaṃ bhagavā avoca.",
"iti73:4.3": "Tatthetaṃ iti vuccati:",
"iti73:5.1": "“Ye ca rūpūpagā sattā,",
"iti73:5.2": "ye ca arūpaṭṭhāyino;",
"iti73:5.3": "Nirodhaṃ appajānantā,",
"iti73:5.4": "āgantāro punabbhavaṃ.",
"iti73:6.1": "Ye ca rūpe pariññāya,",
"iti73:6.2": "arūpesu asaṇṭhitā;",
"iti73:6.3": "Nirodhe ye vimuccanti,",
"iti73:6.4": "te janā maccuhāyino.",
"iti73:7.1": "Kāyena amatadhātuṃ,",
"iti73:7.2": "phusayitvā nirūpadhiṃ;",
"iti73:7.3": "Upadhippaṭinissaggaṃ,",
"iti73:7.4": "sacchikatvā anāsavo;",
"iti73:7.5": "Deseti sammāsambuddho,",
"iti73:7.6": "asokaṃ virajaṃ padan”ti.",
"iti73:8.1": "Ayampi attho vutto bhagavatā, iti me sutanti.",
"iti73:9.1": "Catutthaṃ."
}