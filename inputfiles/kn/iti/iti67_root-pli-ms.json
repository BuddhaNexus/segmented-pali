{
"iti67:1.1": "Itivuttaka 67",
"iti67:2.1": "Moneyyasutta",
"iti67:3.1": "Vuttañhetaṃ bhagavatā vuttamarahatāti me sutaṃ:",
"iti67:4.1": "“Tīṇimāni, bhikkhave, moneyyāni.",
"iti67:4.2": "Katamāni tīṇi?",
"iti67:4.3": "Kāyamoneyyaṃ, vacīmoneyyaṃ, manomoneyyaṃ—",
"iti67:4.4": "imāni kho, bhikkhave, tīṇi moneyyānī”ti.",
"iti67:4.5": "Etamatthaṃ bhagavā avoca.",
"iti67:4.6": "Tatthetaṃ iti vuccati:",
"iti67:5.1": "“Kāyamuniṃ vacīmuniṃ,",
"iti67:5.2": "manomunimanāsavaṃ;",
"iti67:5.3": "Muniṃ moneyyasampannaṃ,",
"iti67:5.4": "āhu ninhātapāpakan”ti.",
"iti67:6.1": "Ayampi attho vutto bhagavatā, iti me sutanti.",
"iti67:7.1": "Aṭṭhamaṃ."
}