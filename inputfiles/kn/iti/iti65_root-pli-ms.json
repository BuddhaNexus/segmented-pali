{
"iti65:1.1": "Itivuttaka 65",
"iti65:2.1": "Sucaritasutta",
"iti65:3.1": "Vuttañhetaṃ bhagavatā vuttamarahatāti me sutaṃ:",
"iti65:4.1": "“Tīṇimāni, bhikkhave, sucaritāni.",
"iti65:4.2": "Katamāni tīṇi?",
"iti65:4.3": "Kāyasucaritaṃ, vacīsucaritaṃ, manosucaritaṃ—",
"iti65:4.4": "imāni kho, bhikkhave, tīṇi sucaritānī”ti.",
"iti65:4.5": "Etamatthaṃ bhagavā avoca.",
"iti65:4.6": "Tatthetaṃ iti vuccati:",
"iti65:5.1": "“Kāyaduccaritaṃ hitvā,",
"iti65:5.2": "vacīduccaritāni ca;",
"iti65:5.3": "Manoduccaritaṃ hitvā,",
"iti65:5.4": "yañcaññaṃ dosasaṃhitaṃ.",
"iti65:6.1": "Akatvākusalaṃ kammaṃ,",
"iti65:6.2": "katvāna kusalaṃ bahuṃ;",
"iti65:6.3": "Kāyassa bhedā sappañño,",
"iti65:6.4": "saggaṃ so upapajjatī”ti.",
"iti65:7.1": "Ayampi attho vutto bhagavatā, iti me sutanti.",
"iti65:8.1": "Chaṭṭhaṃ."
}