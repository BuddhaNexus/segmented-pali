{
"ja166:1.1": "Jātaka",
"ja166:2.1": "Dukanipāta",
"ja166:3.1": "Santhavavagga",
"ja166:4.1": "6. Upasāḷakajātaka",
"ja166:5.1": "“Upasāḷakanāmāni,",
"ja166:5.2": "sahassāni catuddasa;",
"ja166:5.3": "Asmiṃ padese daḍḍhāni,",
"ja166:5.4": "natthi loke anāmataṃ.",
"ja166:6.1": "Yamhi saccañca dhammo ca,",
"ja166:6.2": "ahiṃsā saṃyamo damo;",
"ja166:6.3": "Etaṃ ariyā sevanti,",
"ja166:6.4": "etaṃ loke anāmatan”ti.",
"ja166:7.1": "Upasāḷakajātakaṃ chaṭṭhaṃ."
}