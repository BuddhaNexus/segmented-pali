{
  "ja187:0.1": "Jātaka ",
  "ja187:0.2": "Dukanipāta ",
  "ja187:0.3": "Asadisavagga ",
  "ja187:0.4": "7. Catumaṭṭhajātaka ",
  "ja187:1.1": "“Ucce viṭabhimāruyha, ",
  "ja187:1.2": "mantayavho rahogatā; ",
  "ja187:1.3": "Nīce oruyha mantavho, ",
  "ja187:1.4": "migarājāpi sossati. ",
  "ja187:2.1": "Yaṁ suvaṇṇo suvaṇṇena, ",
  "ja187:2.2": "devo devena mantaye; ",
  "ja187:2.3": "Kiṁ tettha catumaṭṭhassa, ",
  "ja187:2.4": "bilaṁ pavisa jambukā”ti. ",
  "ja187:3.1": "Catumaṭṭhajātakaṁ sattamaṁ. "
}