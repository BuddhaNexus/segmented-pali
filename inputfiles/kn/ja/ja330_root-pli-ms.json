{
"ja330:1.1": "Jātaka",
"ja330:2.1": "Catukkanipāta",
"ja330:3.1": "Kuṭidūsakavagga",
"ja330:4.1": "10. Sīlavīmaṃsajātaka",
"ja330:5.1": "“Sīlaṃ kireva kalyāṇaṃ,",
"ja330:5.2": "sīlaṃ loke anuttaraṃ;",
"ja330:5.3": "Passa ghoraviso nāgo,",
"ja330:5.4": "sīlavāti na haññati”.",
"ja330:6.1": "“Yāvadevassahū kiñci,",
"ja330:6.2": "tāvadeva akhādisuṃ;",
"ja330:6.3": "Saṅgamma kulalā loke,",
"ja330:6.4": "na hiṃsanti akiñcanaṃ.",
"ja330:7.1": "Sukhaṃ nirāsā supati,",
"ja330:7.2": "āsā phalavatī sukhā;",
"ja330:7.3": "Āsaṃ nirāsaṃ katvāna,",
"ja330:7.4": "sukhaṃ supati piṅgalā.",
"ja330:8.1": "Na samādhiparo atthi,",
"ja330:8.2": "asmiṃ loke paramhi ca;",
"ja330:8.3": "Na paraṃ nāpi attānaṃ,",
"ja330:8.4": "vihiṃsati samāhito”ti.",
"ja330:9.1": "Sīlavīmaṃsajātakaṃ dasamaṃ.",
"ja330:9.2": "Kuṭidūsakavaggo tatiyo.",
"ja330:10.1": "Tassuddānaṃ",
"ja330:11.1": "Samanussa saduddubha yācanako,",
"ja330:11.2": "Atha meṇḍavaruttama godhavaro;",
"ja330:11.3": "Atha kāyasakepuka bhotīvaro,",
"ja330:11.4": "Atha rādhasusīlavarena dasāti."
}