{
  "ja330:0.1": "Jātaka ",
  "ja330:0.2": "Catukkanipāta ",
  "ja330:0.3": "Kuṭidūsakavagga ",
  "ja330:0.4": "10. Sīlavīmaṁsajātaka ",
  "ja330:1.1": "“Sīlaṁ kireva kalyāṇaṁ, ",
  "ja330:1.2": "sīlaṁ loke anuttaraṁ; ",
  "ja330:1.3": "Passa ghoraviso nāgo, ",
  "ja330:1.4": "sīlavāti na haññati”. ",
  "ja330:2.1": "“Yāvadevassahū kiñci, ",
  "ja330:2.2": "tāvadeva akhādisuṁ; ",
  "ja330:2.3": "Saṅgamma kulalā loke, ",
  "ja330:2.4": "na hiṁsanti akiñcanaṁ. ",
  "ja330:3.1": "Sukhaṁ nirāsā supati, ",
  "ja330:3.2": "āsā phalavatī sukhā; ",
  "ja330:3.3": "Āsaṁ nirāsaṁ katvāna, ",
  "ja330:3.4": "sukhaṁ supati piṅgalā. ",
  "ja330:4.1": "Na samādhiparo atthi, ",
  "ja330:4.2": "asmiṁ loke paramhi ca; ",
  "ja330:4.3": "Na paraṁ nāpi attānaṁ, ",
  "ja330:4.4": "vihiṁsati samāhito”ti. ",
  "ja330:5.1": "Sīlavīmaṁsajātakaṁ dasamaṁ. ",
  "ja330:5.2": "Kuṭidūsakavaggo tatiyo. ",
  "ja330:6.1": "Tassuddānaṁ ",
  "ja330:7.1": "Samanussa saduddubha yācanako, ",
  "ja330:7.2": "Atha meṇḍavaruttama godhavaro; ",
  "ja330:7.3": "Atha kāyasakepuka bhotīvaro, ",
  "ja330:7.4": "Atha rādhasusīlavarena dasāti. "
}