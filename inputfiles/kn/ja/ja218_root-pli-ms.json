{
"ja218:1.1": "Jātaka",
"ja218:2.1": "Dukanipāta",
"ja218:3.1": "Bīraṇathambhavagga",
"ja218:4.1": "8. Kūṭavāṇijajātaka",
"ja218:5.1": "“Saṭhassa sāṭheyyamidaṃ sucintitaṃ,",
"ja218:5.2": "Paccoḍḍitaṃ paṭikūṭassa kūṭaṃ;",
"ja218:5.3": "Phālañce khādeyyuṃ mūsikā,",
"ja218:5.4": "Kasmā kumāraṃ kulalā na hareyyuṃ.",
"ja218:6.1": "Kūṭassa hi santi kūṭakūṭā,",
"ja218:6.2": "Bhavati cāpi nikatino nikatyā;",
"ja218:6.3": "Dehi puttanaṭṭha phālanaṭṭhassa phālaṃ,",
"ja218:6.4": "Mā te puttamahāsi phālanaṭṭho”ti.",
"ja218:7.1": "Kūṭavāṇijajātakaṃ aṭṭhamaṃ."
}