{
"ja425:1.1": "Jātaka",
"ja425:2.1": "Aṭṭhakanipāta",
"ja425:3.1": "Kaccānivagga",
"ja425:4.1": "9. Aṭṭhānajātaka",
"ja425:5.1": "“Gaṅgā kumudinī santā,",
"ja425:5.2": "saṅkhavaṇṇā ca kokilā;",
"ja425:5.3": "Jambu tālaphalaṃ dajjā,",
"ja425:5.4": "atha nūna tadā siyā.",
"ja425:6.1": "Yadā kacchapalomānaṃ,",
"ja425:6.2": "pāvāro tividho siyā;",
"ja425:6.3": "Hemantikaṃ pāvuraṇaṃ,",
"ja425:6.4": "atha nūna tadā siyā.",
"ja425:7.1": "Yadā makasapādānaṃ,",
"ja425:7.2": "aṭṭālo sukato siyā;",
"ja425:7.3": "Daḷho ca avikampī ca,",
"ja425:7.4": "atha nūna tadā siyā.",
"ja425:8.1": "Yadā sasavisāṇānaṃ,",
"ja425:8.2": "nisseṇī sukatā siyā;",
"ja425:8.3": "Saggassārohaṇatthāya,",
"ja425:8.4": "atha nūna tadā siyā.",
"ja425:9.1": "Yadā nisseṇimāruyha,",
"ja425:9.2": "candaṃ khādeyyu mūsikā;",
"ja425:9.3": "Rāhuñca paripāteyyuṃ,",
"ja425:9.4": "atha nūna tadā siyā.",
"ja425:10.1": "Yadā surāghaṭaṃ pitvā,",
"ja425:10.2": "makkhikā gaṇacāriṇī;",
"ja425:10.3": "Aṅgāre vāsaṃ kappeyyuṃ,",
"ja425:10.4": "atha nūna tadā siyā.",
"ja425:11.1": "Yadā bimboṭṭhasampanno,",
"ja425:11.2": "gadrabho sumukho siyā;",
"ja425:11.3": "Kusalo naccagītassa,",
"ja425:11.4": "atha nūna tadā siyā.",
"ja425:12.1": "Yadā kākā ulūkā ca,",
"ja425:12.2": "mantayeyyuṃ rahogatā;",
"ja425:12.3": "Aññamaññaṃ pihayeyyuṃ,",
"ja425:12.4": "atha nūna tadā siyā.",
"ja425:13.1": "Yadā muḷāla pattānaṃ,",
"ja425:13.2": "chattaṃ thirataraṃ siyā;",
"ja425:13.3": "Vassassa paṭighātāya,",
"ja425:13.4": "atha nūna tadā siyā.",
"ja425:14.1": "Yadā kulako sakuṇo,",
"ja425:14.2": "pabbataṃ gandhamādanaṃ;",
"ja425:14.3": "Tuṇḍenādāya gaccheyya,",
"ja425:14.4": "atha nūna tadā siyā.",
"ja425:15.1": "Yadā sāmuddikaṃ nāvaṃ,",
"ja425:15.2": "sayantaṃ savaṭākaraṃ;",
"ja425:15.3": "Ceṭo ādāya gaccheyya,",
"ja425:15.4": "atha nūna tadā siyā”ti.",
"ja425:16.1": "Aṭṭhānajātakaṃ navamaṃ."
}