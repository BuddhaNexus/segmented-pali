{
  "ja179:0.1": "Jātaka ",
  "ja179:0.2": "Dukanipāta ",
  "ja179:0.3": "Kalyāṇavagga ",
  "ja179:0.4": "9. Satadhammajātaka ",
  "ja179:1.1": "“Tañca appañca ucchiṭṭhaṁ, ",
  "ja179:1.2": "tañca kicchena no adā; ",
  "ja179:1.3": "Sohaṁ brāhmaṇajātiko, ",
  "ja179:1.4": "yaṁ bhuttaṁ tampi uggataṁ”. ",
  "ja179:2.1": "“Evaṁ dhammaṁ niraṅkatvā, ",
  "ja179:2.2": "Yo adhammena jīvati; ",
  "ja179:2.3": "Satadhammova lābhena, ",
  "ja179:2.4": "Laddhenapi na nandatī”ti. ",
  "ja179:3.1": "Satadhammajātakaṁ navamaṁ. "
}