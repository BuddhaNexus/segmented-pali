{
"ja179:1.1": "Jātaka",
"ja179:2.1": "Dukanipāta",
"ja179:3.1": "Kalyāṇavagga",
"ja179:4.1": "9. Satadhammajātaka",
"ja179:5.1": "“Tañca appañca ucchiṭṭhaṃ,",
"ja179:5.2": "tañca kicchena no adā;",
"ja179:5.3": "Sohaṃ brāhmaṇajātiko,",
"ja179:5.4": "yaṃ bhuttaṃ tampi uggataṃ”.",
"ja179:6.1": "“Evaṃ dhammaṃ niraṃkatvā,",
"ja179:6.2": "Yo adhammena jīvati;",
"ja179:6.3": "Satadhammova lābhena,",
"ja179:6.4": "Laddhenapi na nandatī”ti.",
"ja179:7.1": "Satadhammajātakaṃ navamaṃ."
}