{
"ja411:1.1": "Jātaka",
"ja411:2.1": "Sattakanipāta",
"ja411:3.1": "Gandhāravagga",
"ja411:4.1": "6. Susīmajātaka",
"ja411:5.1": "“Kāḷāni kesāni pure ahesuṃ,",
"ja411:5.2": "Jātāni sīsamhi yathāpadese;",
"ja411:5.3": "Tānajja setāni susīma disvā,",
"ja411:5.4": "Dhammaṃ cara brahmacariyassa kālo”.",
"ja411:6.1": "“Mameva deva palitaṃ na tuyhaṃ,",
"ja411:6.2": "Mameva sīsaṃ mama uttamaṅgaṃ;",
"ja411:6.3": "‘Atthaṃ karissan’ti musā abhāṇiṃ,",
"ja411:6.4": "Ekāparādhaṃ khama rājaseṭṭha.",
"ja411:7.1": "Daharo tuvaṃ dassaniyosi rāja,",
"ja411:7.2": "Paṭhamuggato hosi yathā kaḷīro;",
"ja411:7.3": "Rajjañca kārehi mamañca passa,",
"ja411:7.4": "Mā kālikaṃ anudhāvī janinda”.",
"ja411:8.1": "“Passāmi vohaṃ dahariṃ kumāriṃ,",
"ja411:8.2": "Sāmaṭṭhapassaṃ sutanuṃ sumajjhaṃ;",
"ja411:8.3": "Kāḷappavāḷāva pavellamānā,",
"ja411:8.4": "Palobhayantīva naresu gacchati.",
"ja411:9.1": "Tamena passāmiparena nāriṃ,",
"ja411:9.2": "Āsītikaṃ nāvutikaṃ va jaccā;",
"ja411:9.3": "Daṇḍaṃ gahetvāna pavedhamānaṃ,",
"ja411:9.4": "Gopānasībhoggasamaṃ carantiṃ.",
"ja411:10.1": "Sohaṃ tamevānuvicintayanto,",
"ja411:10.2": "Eko sayāmi sayanassa majjhe;",
"ja411:10.3": "‘Ahampi evaṃ’ iti pekkhamāno,",
"ja411:10.4": "Na gahe rame brahmacariyassa kālo.",
"ja411:11.1": "Rajjuvālambanī cesā,",
"ja411:11.2": "Yā gehe vasato rati;",
"ja411:11.3": "Evampi chetvāna vajanti dhīrā,",
"ja411:11.4": "Anapekkhino kāmasukhaṃ pahāyā”ti.",
"ja411:12.1": "Susīmajātakaṃ chaṭṭhaṃ."
}