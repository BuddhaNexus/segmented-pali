{
"ja390:1.1": "Jātaka",
"ja390:2.1": "Chakkanipāta",
"ja390:3.1": "Kharaputtavagga",
"ja390:4.1": "5. Mayhakajātaka",
"ja390:5.1": "“Sakuṇo mayhako nāma,",
"ja390:5.2": "girisānudarīcaro;",
"ja390:5.3": "Pakkaṃ pipphalimāruyha,",
"ja390:5.4": "mayhaṃ mayhanti kandati.",
"ja390:6.1": "Tassevaṃ vilapantassa,",
"ja390:6.2": "dijasaṅghā samāgatā;",
"ja390:6.3": "Bhutvāna pipphaliṃ yanti,",
"ja390:6.4": "vilapatveva so dijo.",
"ja390:7.1": "Evameva idhekacco,",
"ja390:7.2": "saṅgharitvā bahuṃ dhanaṃ;",
"ja390:7.3": "Nevattano na ñātīnaṃ,",
"ja390:7.4": "yathodhiṃ paṭipajjati.",
"ja390:8.1": "Na so acchādanaṃ bhattaṃ,",
"ja390:8.2": "na mālaṃ na vilepanaṃ;",
"ja390:8.3": "Anubhoti sakiṃ kiñci,",
"ja390:8.4": "na saṅgaṇhāti ñātake.",
"ja390:9.1": "Tassevaṃ vilapantassa,",
"ja390:9.2": "mayhaṃ mayhanti rakkhato;",
"ja390:9.3": "Rājāno atha vā corā,",
"ja390:9.4": "dāyādā ye va appiyā;",
"ja390:9.5": "Dhanamādāya gacchanti,",
"ja390:9.6": "vilapatveva so naro.",
"ja390:10.1": "Dhīro bhoge adhigamma,",
"ja390:10.2": "saṅgaṇhāti ca ñātake;",
"ja390:10.3": "Tena so kittiṃ pappoti,",
"ja390:10.4": "pecca sagge pamodatī”ti.",
"ja390:11.1": "Mayhakajātakaṃ pañcamaṃ."
}