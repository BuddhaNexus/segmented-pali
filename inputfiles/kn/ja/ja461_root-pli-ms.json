{
"ja461:1.1": "Jātaka",
"ja461:2.1": "Ekādasakanipāta",
"ja461:3.1": "Mātuposakavagga",
"ja461:4.1": "7. Dasarathajātaka",
"ja461:5.1": "“Etha lakkhaṇa sītā ca,",
"ja461:5.2": "ubho otarathodakaṃ;",
"ja461:5.3": "Evāyaṃ bharato āha,",
"ja461:5.4": "‘rājā dasaratho mato’”.",
"ja461:6.1": "“Kena rāmappabhāvena,",
"ja461:6.2": "socitabbaṃ na socasi;",
"ja461:6.3": "Pitaraṃ kālakataṃ sutvā,",
"ja461:6.4": "na taṃ pasahate dukhaṃ”.",
"ja461:7.1": "“Yaṃ na sakkā nipāletuṃ,",
"ja461:7.2": "posena lapataṃ bahuṃ;",
"ja461:7.3": "Sa kissa viññū medhāvī,",
"ja461:7.4": "attānamupatāpaye.",
"ja461:8.1": "Daharā ca hi vuddhā ca,",
"ja461:8.2": "ye bālā ye ca paṇḍitā;",
"ja461:8.3": "Aḍḍhā ceva daliddā ca,",
"ja461:8.4": "sabbe maccuparāyaṇā.",
"ja461:9.1": "Phalānamiva pakkānaṃ,",
"ja461:9.2": "niccaṃ patanato bhayaṃ;",
"ja461:9.3": "Evaṃ jātāna maccānaṃ,",
"ja461:9.4": "nicca maraṇato bhayaṃ.",
"ja461:10.1": "Sāyameke na dissanti,",
"ja461:10.2": "pāto diṭṭhā bahujjanā;",
"ja461:10.3": "Pāto eke na dissanti,",
"ja461:10.4": "sāyaṃ diṭṭhā bahujjanā.",
"ja461:11.1": "Paridevayamāno ce,",
"ja461:11.2": "kiñcidatthaṃ udabbahe;",
"ja461:11.3": "Sammūḷho hiṃsamattānaṃ,",
"ja461:11.4": "kayirā taṃ vicakkhaṇo.",
"ja461:12.1": "Kiso vivaṇṇo bhavati,",
"ja461:12.2": "hiṃsamattānamattano;",
"ja461:12.3": "Na tena petā pālenti,",
"ja461:12.4": "niratthā paridevanā.",
"ja461:13.1": "Yathā saraṇamādittaṃ,",
"ja461:13.2": "vārinā parinibbaye;",
"ja461:13.3": "Evampi dhīro sutavā,",
"ja461:13.4": "medhāvī paṇḍito naro;",
"ja461:13.5": "Khippamuppatitaṃ sokaṃ,",
"ja461:13.6": "vāto tūlaṃva dhaṃsaye.",
"ja461:14.1": "Macco ekova acceti,",
"ja461:14.2": "ekova jāyate kule;",
"ja461:14.3": "Saṃyogaparamā tveva,",
"ja461:14.4": "sambhogā sabbapāṇinaṃ.",
"ja461:15.1": "Tasmā hi dhīrassa bahussutassa,",
"ja461:15.2": "Sampassato lokamimaṃ parañca;",
"ja461:15.3": "Aññāya dhammaṃ hadayaṃ manañca,",
"ja461:15.4": "Sokā mahantāpi na tāpayanti.",
"ja461:16.1": "Sohaṃ dassañca bhokkhañca,",
"ja461:16.2": "bharissāmi ca ñātake;",
"ja461:16.3": "Sesañca pālayissāmi,",
"ja461:16.4": "kiccametaṃ vijānato.",
"ja461:17.1": "Dasa vassasahassāni,",
"ja461:17.2": "saṭṭhi vassasatāni ca;",
"ja461:17.3": "Kambugīvo mahābāhu,",
"ja461:17.4": "rāmo rajjamakārayī”ti.",
"ja461:18.1": "Dasarathajātakaṃ sattamaṃ."
}