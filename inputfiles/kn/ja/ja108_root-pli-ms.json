{
"ja108:1.1": "Jātaka",
"ja108:2.1": "Ekakanipāta",
"ja108:3.1": "Parosatavagga",
"ja108:4.1": "8. Bāhiyajātaka",
"ja108:5.1": "“Sikkheyya sikkhitabbāni,",
"ja108:5.2": "santi sacchandino janā;",
"ja108:5.3": "Bāhiyā hi suhannena,",
"ja108:5.4": "rājānamabhirādhayī”ti.",
"ja108:6.1": "Bāhiyajātakaṃ aṭṭhamaṃ."
}