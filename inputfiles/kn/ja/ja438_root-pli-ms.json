{
"ja438:1.1": "Jātaka",
"ja438:2.1": "Navakanipāta",
"ja438:3.1": "Gijjhavagga",
"ja438:4.1": "12. Daddarajātaka",
"ja438:5.1": "“Yo te puttake akhādi,",
"ja438:5.2": "dinnabhatto adūsake;",
"ja438:5.3": "Tasmiṃ dāṭhaṃ nipātehi,",
"ja438:5.4": "mā te muccittha jīvato”.",
"ja438:6.1": "“Ākiṇṇaluddo puriso,",
"ja438:6.2": "dhāticelaṃva makkhito;",
"ja438:6.3": "Padesaṃ taṃ na passāmi,",
"ja438:6.4": "yattha dāṭhaṃ nipātaye.",
"ja438:7.1": "Akataññussa posassa,",
"ja438:7.2": "niccaṃ vivaradassino;",
"ja438:7.3": "Sabbañce pathaviṃ dajjā,",
"ja438:7.4": "neva naṃ abhirādhaye”.",
"ja438:8.1": "“Kiṃ nu subāhu taramānarūpo,",
"ja438:8.2": "Paccāgatosi saha māṇavena;",
"ja438:8.3": "Kiṃ kiccamatthaṃ idhamatthi tuyhaṃ,",
"ja438:8.4": "Akkhāhi me pucchito etamatthaṃ”.",
"ja438:9.1": "“Yo te sakhā daddaro sādhurūpo,",
"ja438:9.2": "Tassa vadhaṃ parisaṅkāmi ajja;",
"ja438:9.3": "Purisassa kammāyatanāni sutvā,",
"ja438:9.4": "Nāhaṃ sukhiṃ daddaraṃ ajja maññe”.",
"ja438:10.1": "“Kānissa kammāyatanāni assu,",
"ja438:10.2": "Purisassa vuttisamodhānatāya;",
"ja438:10.3": "Kaṃ vā paṭiññaṃ purisassa sutvā,",
"ja438:10.4": "Parisaṅkasi daddaraṃ māṇavena”.",
"ja438:11.1": "“Ciṇṇā kaliṅgā caritā vaṇijjā,",
"ja438:11.2": "Vettācaro saṅkupathopi ciṇṇo;",
"ja438:11.3": "Naṭehi ciṇṇaṃ saha vākurehi,",
"ja438:11.4": "Daṇḍena yuddhampi samajjamajjhe.",
"ja438:12.1": "Baddhā kulīkā mitamāḷhakena,",
"ja438:12.2": "Akkhā jitā saṃyamo abbhatīto;",
"ja438:12.3": "Abbāhitaṃ pubbakaṃ aḍḍharattaṃ,",
"ja438:12.4": "Hatthā daḍḍhā piṇḍapaṭiggahena.",
"ja438:13.1": "Tānissa kammāyatanāni assu,",
"ja438:13.2": "Purisassa vuttisamodhānatāya;",
"ja438:13.3": "Yathā ayaṃ dissati lomapiṇḍo,",
"ja438:13.4": "Gāvo hatā kiṃ pana daddarassā”ti. #1348",
"ja438:14.1": "Daddarajātakaṃ dvādasamaṃ.",
"ja438:15.1": "Navakanipātaṃ niṭṭhitaṃ.",
"ja438:16.1": "Tassuddānaṃ",
"ja438:17.1": "Varagijjha samajjana haṃsavaro,",
"ja438:17.2": "Nidhisavhaya hārita pāṭaliko;",
"ja438:17.3": "Ajarāmara dhaṅka titikkha kuto,",
"ja438:17.4": "Atha dvādasa pekkhana daddaribhīti."
}