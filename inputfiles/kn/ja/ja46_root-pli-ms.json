{
"ja46:1.1": "Jātaka",
"ja46:2.1": "Ekakanipāta",
"ja46:3.1": "Atthakāmavagga",
"ja46:4.1": "6. Ārāmadūsakajātaka",
"ja46:5.1": "“Na ve anatthakusalena,",
"ja46:5.2": "atthacariyā sukhāvahā;",
"ja46:5.3": "Hāpeti atthaṃ dummedho,",
"ja46:5.4": "kapi ārāmiko yathā”ti.",
"ja46:6.1": "Ārāmadūsakajātakaṃ chaṭṭhaṃ."
}