{
"ja183:1.1": "Jātaka",
"ja183:2.1": "Dukanipāta",
"ja183:3.1": "Asadisavagga",
"ja183:4.1": "3. Vālodakajātaka",
"ja183:5.1": "“Vālodakaṃ apparasaṃ nihīnaṃ,",
"ja183:5.2": "Pitvā mado jāyati gadrabhānaṃ;",
"ja183:5.3": "Imañca pitvāna rasaṃ paṇītaṃ,",
"ja183:5.4": "Mado na sañjāyati sindhavānaṃ”.",
"ja183:6.1": "“Appaṃ pivitvāna nihīnajacco,",
"ja183:6.2": "So majjatī tena janinda puṭṭho;",
"ja183:6.3": "Dhorayhasīlī ca kulamhi jāto,",
"ja183:6.4": "Na majjatī aggarasaṃ pivitvā”ti.",
"ja183:7.1": "Vālodakajātakaṃ tatiyaṃ."
}