{
  "ja517:0.1": "Jātaka ",
  "ja517:0.2": "Tiṁsanipāta ",
  "ja517:0.3": "Kiṁchandavagga ",
  "ja517:0.4": "7. Dakarakkhasajātaka ",
  "ja517:1.1": "“Sace vo vuyhamānānaṁ, ",
  "ja517:1.2": "sattannaṁ udakaṇṇave; ",
  "ja517:1.3": "Manussabalimesāno, ",
  "ja517:1.4": "nāvaṁ gaṇheyya rakkhaso; ",
  "ja517:1.5": "Anupubbaṁ kathaṁ datvā, ",
  "ja517:1.6": "muñcesi dakarakkhasā”. ",
  "ja517:2.1": "“Mātaraṁ paṭhamaṁ dajjaṁ, ",
  "ja517:2.2": "Bhariyaṁ datvāna bhātaraṁ; ",
  "ja517:2.3": "Tato sahāyaṁ datvāna, ",
  "ja517:2.4": "Pañcamaṁ dajjaṁ brāhmaṇaṁ; ",
  "ja517:2.5": "Chaṭṭhāhaṁ dajjamattānaṁ, ",
  "ja517:2.6": "Neva dajjaṁ mahosadhaṁ”. ",
  "ja517:3.1": "“Posetā te janettī ca, ",
  "ja517:3.2": "dīgharattānukampikā; ",
  "ja517:3.3": "Chabbhī tayi padussati, ",
  "ja517:3.4": "paṇḍitā atthadassinī; ",
  "ja517:3.5": "Aññaṁ upanisaṁ katvā, ",
  "ja517:3.6": "vadhā taṁ parimocayi. ",
  "ja517:4.1": "Taṁ tādisiṁ pāṇadadiṁ, ",
  "ja517:4.2": "orasaṁ gabbhadhāriniṁ; ",
  "ja517:4.3": "Mātaraṁ kena dosena, ",
  "ja517:4.4": "dajjāsi dakarakkhino”. ",
  "ja517:5.1": "“Daharā viyalaṅkāraṁ, ",
  "ja517:5.2": "dhāreti apiḷandhanaṁ; ",
  "ja517:5.3": "Dovārike anīkaṭṭhe, ",
  "ja517:5.4": "ativelaṁ pajagghati. ",
  "ja517:6.1": "Athopi paṭirājūnaṁ, ",
  "ja517:6.2": "sayaṁ dūtāni sāsati; ",
  "ja517:6.3": "Mātaraṁ tena dosena, ",
  "ja517:6.4": "dajjāhaṁ dakarakkhino”. ",
  "ja517:7.1": "“Itthigumbassa pavarā, ",
  "ja517:7.2": "accantaṁ piyabhāṇinī; ",
  "ja517:7.3": "Anuggatā sīlavatī, ",
  "ja517:7.4": "chāyāva anapāyinī. ",
  "ja517:8.1": "Akkodhanā puññavatī, ",
  "ja517:8.2": "paṇḍitā atthadassinī; ",
  "ja517:8.3": "Ubbariṁ kena dosena, ",
  "ja517:8.4": "dajjāsi dakarakkhino”. ",
  "ja517:9.1": "“Khiḍḍāratisamāpannaṁ, ",
  "ja517:9.2": "anatthavasamāgataṁ; ",
  "ja517:9.3": "Sā maṁ sakāna puttānaṁ, ",
  "ja517:9.4": "ayācaṁ yācate dhanaṁ. ",
  "ja517:10.1": "Sohaṁ dadāmi sāratto, ",
  "ja517:10.2": "bahuṁ uccāvacaṁ dhanaṁ; ",
  "ja517:10.3": "Suduccajaṁ cajitvāna, ",
  "ja517:10.4": "pacchā socāmi dummano; ",
  "ja517:10.5": "Ubbariṁ tena dosena, ",
  "ja517:10.6": "dajjāhaṁ dakarakkhino”. ",
  "ja517:11.1": "“Yenocitā janapadā, ",
  "ja517:11.2": "ānītā ca paṭiggahaṁ; ",
  "ja517:11.3": "Ābhataṁ pararajjebhi, ",
  "ja517:11.4": "abhiṭṭhāya bahuṁ dhanaṁ. ",
  "ja517:12.1": "Dhanuggahānaṁ pavaraṁ, ",
  "ja517:12.2": "sūraṁ tikhiṇamantinaṁ; ",
  "ja517:12.3": "Bhātaraṁ kena dosena, ",
  "ja517:12.4": "dajjāsi dakarakkhino”. ",
  "ja517:13.1": "“Yenocitā janapadā, ",
  "ja517:13.2": "ānītā ca paṭiggahaṁ; ",
  "ja517:13.3": "Ābhataṁ pararajjebhi, ",
  "ja517:13.4": "abhiṭṭhāya bahuṁ dhanaṁ. ",
  "ja517:14.1": "Dhanuggahānaṁ pavaro, ",
  "ja517:14.2": "sūro tikhiṇamanti ca; ",
  "ja517:14.3": "Mayāyaṁ sukhito rājā, ",
  "ja517:14.4": "atimaññati dārako. ",
  "ja517:15.1": "Upaṭṭhānampi me ayye, ",
  "ja517:15.2": "na so eti yathā pure; ",
  "ja517:15.3": "Bhātaraṁ tena dosena, ",
  "ja517:15.4": "dajjāhaṁ dakarakkhino”. ",
  "ja517:16.1": "“Ekarattena ubhayo, ",
  "ja517:16.2": "tvañceva dhanusekha ca; ",
  "ja517:16.3": "Ubho jātettha pañcālā, ",
  "ja517:16.4": "sahāyā susamāvayā. ",
  "ja517:17.1": "Cariyā taṁ anubandhittho, ",
  "ja517:17.2": "ekadukkhasukho tava; ",
  "ja517:17.3": "Ussukko te divārattiṁ, ",
  "ja517:17.4": "sabbakiccesu byāvaṭo; ",
  "ja517:17.5": "Sahāyaṁ kena dosena, ",
  "ja517:17.6": "dajjāsi dakarakkhino”. ",
  "ja517:18.1": "“Cariyā maṁ ayaṁ ayye, ",
  "ja517:18.2": "pajagghittho mayā saha; ",
  "ja517:18.3": "Ajjāpi tena vaṇṇena, ",
  "ja517:18.4": "ativelaṁ pajagghati. ",
  "ja517:19.1": "Ubbariyāpihaṁ ayye, ",
  "ja517:19.2": "mantayāmi rahogato; ",
  "ja517:19.3": "Anāmanto pavisati, ",
  "ja517:19.4": "pubbe appaṭivedito. ",
  "ja517:20.1": "Laddhadvāro katokāso, ",
  "ja517:20.2": "ahirikaṁ anādaraṁ; ",
  "ja517:20.3": "Sahāyaṁ tena dosena, ",
  "ja517:20.4": "dajjāhaṁ dakarakkhino”. ",
  "ja517:21.1": "“Kusalo sabbanimittānaṁ, ",
  "ja517:21.2": "rutaññū āgatāgamo; ",
  "ja517:21.3": "Uppāte supine yutto, ",
  "ja517:21.4": "niyyāne ca pavesane. ",
  "ja517:22.1": "Paṭṭho bhūmantalikkhasmiṁ, ",
  "ja517:22.2": "Nakkhattapadakovido; ",
  "ja517:22.3": "Brāhmaṇaṁ kena dosena, ",
  "ja517:22.4": "Dajjāsi dakarakkhino”. ",
  "ja517:23.1": "“Parisāyampi me ayye, ",
  "ja517:23.2": "ummīlitvā udikkhati; ",
  "ja517:23.3": "Tasmā accabhamuṁ luddaṁ, ",
  "ja517:23.4": "dajjāhaṁ dakarakkhino”. ",
  "ja517:24.1": "“Sasamuddapariyāyaṁ, ",
  "ja517:24.2": "mahiṁ sāgarakuṇḍalaṁ; ",
  "ja517:24.3": "Vasundharaṁ āvasasi, ",
  "ja517:24.4": "amaccaparivārito. ",
  "ja517:25.1": "Cāturanto mahāraṭṭho, ",
  "ja517:25.2": "vijitāvī mahabbalo; ",
  "ja517:25.3": "Pathabyā ekarājāsi, ",
  "ja517:25.4": "yaso te vipulaṁ gato. ",
  "ja517:26.1": "Soḷasitthisahassāni, ",
  "ja517:26.2": "āmuttamaṇikuṇḍalā; ",
  "ja517:26.3": "Nānājanapadā nārī, ",
  "ja517:26.4": "devakaññūpamā subhā. ",
  "ja517:27.1": "Evaṁ sabbaṅgasampannaṁ, ",
  "ja517:27.2": "sabbakāmasamiddhinaṁ; ",
  "ja517:27.3": "Sukhitānaṁ piyaṁ dīghaṁ, ",
  "ja517:27.4": "jīvitaṁ āhu khattiya. ",
  "ja517:28.1": "Atha tvaṁ kena vaṇṇena, ",
  "ja517:28.2": "kena vā pana hetunā; ",
  "ja517:28.3": "Paṇḍitaṁ anurakkhanto, ",
  "ja517:28.4": "pāṇaṁ cajasi duccajaṁ”. ",
  "ja517:29.1": "“Yatopi āgato ayye, ",
  "ja517:29.2": "mama hatthaṁ mahosadho; ",
  "ja517:29.3": "Nābhijānāmi dhīrassa, ",
  "ja517:29.4": "anumattampi dukkaṭaṁ. ",
  "ja517:30.1": "Sace ca kismici kāle, ",
  "ja517:30.2": "maraṇaṁ me pure siyā; ",
  "ja517:30.3": "So me putte paputte ca, ",
  "ja517:30.4": "sukhāpeyya mahosadho. ",
  "ja517:31.1": "Anāgataṁ paccuppannaṁ, ",
  "ja517:31.2": "sabbamatthampi passati; ",
  "ja517:31.3": "Anāparādhakammantaṁ, ",
  "ja517:31.4": "na dajjaṁ dakarakkhino”. ",
  "ja517:32.1": "“Idaṁ suṇātha pañcālā, ",
  "ja517:32.2": "cūḷaneyyassa bhāsitaṁ; ",
  "ja517:32.3": "Paṇḍitaṁ anurakkhanto, ",
  "ja517:32.4": "pāṇaṁ cajati duccajaṁ. ",
  "ja517:33.1": "Mātu bhariyāya bhātucca, ",
  "ja517:33.2": "sakhino brāhmaṇassa ca; ",
  "ja517:33.3": "Attano cāpi pañcālo, ",
  "ja517:33.4": "channaṁ cajati jīvitaṁ. ",
  "ja517:34.1": "Evaṁ mahatthikā paññā, ",
  "ja517:34.2": "nipuṇā sādhucintinī; ",
  "ja517:34.3": "Diṭṭhadhammahitatthāya, ",
  "ja517:34.4": "samparāyasukhāya cā”ti. ",
  "ja517:35.1": "Dakarakkhasajātakaṁ sattamaṁ. "
}