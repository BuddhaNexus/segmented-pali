{
  "ja286:0.1": "Jātaka ",
  "ja286:0.2": "Tikanipāta ",
  "ja286:0.3": "Abbhantaravagga ",
  "ja286:0.4": "6. Sālūkajātaka ",
  "ja286:1.1": "“Mā sālūkassa pihayi, ",
  "ja286:1.2": "āturannāni bhuñjati; ",
  "ja286:1.3": "Appossukko bhusaṁ khāda, ",
  "ja286:1.4": "etaṁ dīghāyulakkhaṇaṁ. ",
  "ja286:2.1": "Idāni so idhāgantvā, ",
  "ja286:2.2": "atithī yuttasevako; ",
  "ja286:2.3": "Atha dakkhasi sālūkaṁ, ",
  "ja286:2.4": "sayantaṁ musaluttaraṁ”. ",
  "ja286:3.1": "Vikantaṁ sūkaraṁ disvā, ",
  "ja286:3.2": "sayantaṁ musaluttaraṁ; ",
  "ja286:3.3": "Jaraggavā vicintesuṁ, ",
  "ja286:3.4": "varamhākaṁ bhusāmivāti. ",
  "ja286:4.1": "Sālūkajātakaṁ chaṭṭhaṁ. "
}