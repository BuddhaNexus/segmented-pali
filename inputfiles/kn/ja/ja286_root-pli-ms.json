{
"ja286:1.1": "Jātaka",
"ja286:2.1": "Tikanipāta",
"ja286:3.1": "Abbhantaravagga",
"ja286:4.1": "6. Sālūkajātaka",
"ja286:5.1": "“Mā sālūkassa pihayi,",
"ja286:5.2": "āturannāni bhuñjati;",
"ja286:5.3": "Appossukko bhusaṃ khāda,",
"ja286:5.4": "etaṃ dīghāyulakkhaṇaṃ.",
"ja286:6.1": "Idāni so idhāgantvā,",
"ja286:6.2": "atithī yuttasevako;",
"ja286:6.3": "Atha dakkhasi sālūkaṃ,",
"ja286:6.4": "sayantaṃ musaluttaraṃ”.",
"ja286:7.1": "Vikantaṃ sūkaraṃ disvā,",
"ja286:7.2": "sayantaṃ musaluttaraṃ;",
"ja286:7.3": "Jaraggavā vicintesuṃ,",
"ja286:7.4": "varamhākaṃ bhusāmivāti.",
"ja286:8.1": "Sālūkajātakaṃ chaṭṭhaṃ."
}