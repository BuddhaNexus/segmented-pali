{
"ja165:1.1": "Jātaka",
"ja165:2.1": "Dukanipāta",
"ja165:3.1": "Santhavavagga",
"ja165:4.1": "5. Nakulajātaka",
"ja165:5.1": "“Sandhiṃ katvā amittena,",
"ja165:5.2": "aṇḍajena jalābuja;",
"ja165:5.3": "Vivariya dāṭhaṃ sesi,",
"ja165:5.4": "kuto te bhayamāgataṃ”.",
"ja165:6.1": "“Saṅketheva amittasmiṃ,",
"ja165:6.2": "mittasmimpi na vissase;",
"ja165:6.3": "Abhayā bhayamuppannaṃ,",
"ja165:6.4": "api mūlāni kantatī”ti.",
"ja165:7.1": "Nakulajātakaṃ pañcamaṃ."
}