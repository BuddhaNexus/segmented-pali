{
"ja377:1.1": "Jātaka",
"ja377:2.1": "Chakkanipāta",
"ja377:3.1": "Avāriyavagga",
"ja377:4.1": "2. Setaketujātaka",
"ja377:5.1": "“Mā tāta kujjhi na hi sādhu kodho,",
"ja377:5.2": "Bahumpi te adiṭṭhamassutañca;",
"ja377:5.3": "Mātā pitā disatā setaketu,",
"ja377:5.4": "Ācariyamāhu disataṃ pasatthā.",
"ja377:6.1": "Agārino annadapānavatthadā,",
"ja377:6.2": "Avhāyikā tampi disaṃ vadanti;",
"ja377:6.3": "Esā disā paramā setaketu,",
"ja377:6.4": "Yaṃ patvā dukkhī sukhino bhavanti”.",
"ja377:7.1": "“Kharājinā jaṭilā paṅkadantā,",
"ja377:7.2": "Dummakkharūpā yeme jappanti mante;",
"ja377:7.3": "Kacci nu te mānusake payoge,",
"ja377:7.4": "Idaṃ vidū parimuttā apāyā”.",
"ja377:8.1": "“Pāpāni kammāni katvāna rāja,",
"ja377:8.2": "Bahussuto ce na careyya dhammaṃ;",
"ja377:8.3": "Sahassavedopi na taṃ paṭicca,",
"ja377:8.4": "Dukkhā pamucce caraṇaṃ apatvā”.",
"ja377:9.1": "“Sahassavedopi na taṃ paṭicca,",
"ja377:9.2": "Dukkhā pamucce caraṇaṃ apatvā;",
"ja377:9.3": "Maññāmi vedā aphalā bhavanti,",
"ja377:9.4": "Sasaṃyamaṃ caraṇameva saccaṃ”.",
"ja377:10.1": "“Na heva vedā aphalā bhavanti,",
"ja377:10.2": "Sasaṃyamaṃ caraṇameva saccaṃ;",
"ja377:10.3": "Kittiñhi pappoti adhicca vede,",
"ja377:10.4": "Santiṃ puṇeti caraṇena danto”ti.",
"ja377:11.1": "Setaketujātakaṃ dutiyaṃ."
}