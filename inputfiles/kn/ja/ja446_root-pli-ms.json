{
"ja446:1.1": "Jātaka",
"ja446:2.1": "Dasakanipāta",
"ja446:3.1": "Catudvāravagga",
"ja446:4.1": "8. Takkalajātaka",
"ja446:5.1": "“Na takkalā santi na āluvāni,",
"ja446:5.2": "Na biḷāliyo na kaḷambāni tāta;",
"ja446:5.3": "Eko araññamhi susānamajjhe,",
"ja446:5.4": "Kimatthiko tāta khaṇāsi kāsuṃ”.",
"ja446:6.1": "“Pitāmaho tāta sudubbalo te,",
"ja446:6.2": "Anekabyādhīhi dukhena phuṭṭho;",
"ja446:6.3": "Tamajjahaṃ nikhaṇissāmi sobbhe,",
"ja446:6.4": "Na hissa taṃ jīvitaṃ rocayāmi”.",
"ja446:7.1": "“Saṅkappametaṃ paṭiladdha pāpakaṃ,",
"ja446:7.2": "Accāhitaṃ kamma karosi luddaṃ;",
"ja446:7.3": "Mayāpi tāta paṭilacchase tuvaṃ,",
"ja446:7.4": "Etādisaṃ kamma jarūpanīto;",
"ja446:7.5": "Taṃ kullavattaṃ anuvattamāno,",
"ja446:7.6": "Ahampi taṃ nikhaṇissāmi sobbhe”.",
"ja446:8.1": "“Pharusāhi vācāhi pakubbamāno,",
"ja446:8.2": "Āsajja maṃ tvaṃ vadase kumāra;",
"ja446:8.3": "Putto mamaṃ orasako samāno,",
"ja446:8.4": "Ahitānukampī mama tvaṃsi putta”.",
"ja446:9.1": "“Na tāhaṃ tāta ahitānukampī,",
"ja446:9.2": "Hitānukampī te ahampi tāta;",
"ja446:9.3": "Pāpañca taṃ kamma pakubbamānaṃ,",
"ja446:9.4": "Arahāmi no vārayituṃ tato”.",
"ja446:10.1": "“Yo mātaraṃ vā pitaraṃ saviṭṭha,",
"ja446:10.2": "Adūsake hiṃsati pāpadhammo;",
"ja446:10.3": "Kāyassa bhedā abhisamparāyaṃ,",
"ja446:10.4": "Asaṃsayaṃ so nirayaṃ upeti.",
"ja446:11.1": "Yo mātaraṃ vā pitaraṃ saviṭṭha,",
"ja446:11.2": "Annena pānena upaṭṭhahāti;",
"ja446:11.3": "Kāyassa bhedā abhisamparāyaṃ,",
"ja446:11.4": "Asaṃsayaṃ so sugatiṃ upeti.",
"ja446:12.1": "Na me tvaṃ putta ahitānukampī,",
"ja446:12.2": "Hitānukampī me tvaṃsi putta;",
"ja446:12.3": "Ahañca taṃ mātarā vuccamāno,",
"ja446:12.4": "Etādisaṃ kamma karomi luddaṃ”.",
"ja446:13.1": "“Yā te sā bhariyā anariyarūpā,",
"ja446:13.2": "Mātā mamesā sakiyā janetti;",
"ja446:13.3": "Niddhāpaye tañca sakā agārā,",
"ja446:13.4": "Aññampi te sā dukhamāvaheyya.",
"ja446:14.1": "Yā te sā bhariyā anariyarūpā,",
"ja446:14.2": "Mātā mamesā sakiyā janetti;",
"ja446:14.3": "Dantā kareṇūva vasūpanītā,",
"ja446:14.4": "Sā pāpadhammā punarāvajātū”ti.",
"ja446:15.1": "Takkalajātakaṃ aṭṭhamaṃ."
}