{
"ja241:1.1": "Jātaka",
"ja241:2.1": "Dukanipāta",
"ja241:3.1": "Siṅgālavagga",
"ja241:4.1": "1. Sabbadāṭhijātaka",
"ja241:5.1": "“Siṅgālo mānathaddho ca,",
"ja241:5.2": "parivārena atthiko;",
"ja241:5.3": "Pāpuṇi mahatiṃ bhūmiṃ,",
"ja241:5.4": "rājāsi sabbadāṭhinaṃ.",
"ja241:6.1": "Evameva manussesu,",
"ja241:6.2": "yo hoti parivāravā;",
"ja241:6.3": "So hi tattha mahā hoti,",
"ja241:6.4": "siṅgālo viya dāṭhinan”ti.",
"ja241:7.1": "Sabbadāṭhijātakaṃ paṭhamaṃ."
}