{
"ja478:1.1": "Jātaka",
"ja478:2.1": "Terasakanipāta",
"ja478:3.1": "Ambavagga",
"ja478:4.1": "5. Dūtajātaka",
"ja478:5.1": "“Dūte te brahme pāhesiṃ,",
"ja478:5.2": "Gaṅgātīrasmi jhāyato;",
"ja478:5.3": "Tesaṃ puṭṭho na byākāsi,",
"ja478:5.4": "Dukkhaṃ guyhamataṃ nu te”.",
"ja478:6.1": "“Sace te dukkhamuppajje,",
"ja478:6.2": "kāsīnaṃ raṭṭhavaḍḍhana;",
"ja478:6.3": "Mā kho naṃ tassa akkhāhi,",
"ja478:6.4": "yo taṃ dukkhā na mocaye.",
"ja478:7.1": "Yo tassa dukkhajātassa,",
"ja478:7.2": "ekaṅgamapi bhāgaso;",
"ja478:7.3": "Vippamoceyya dhammena,",
"ja478:7.4": "kāmaṃ tassa pavedaya.",
"ja478:8.1": "Suvijānaṃ siṅgālānaṃ,",
"ja478:8.2": "sakuṇānañca vassitaṃ;",
"ja478:8.3": "Manussavassitaṃ rāja,",
"ja478:8.4": "dubbijānataraṃ tato.",
"ja478:9.1": "Api ce maññatī poso,",
"ja478:9.2": "ñāti mitto sakhāti vā;",
"ja478:9.3": "Yo pubbe sumano hutvā,",
"ja478:9.4": "pacchā sampajjate diso.",
"ja478:10.1": "Yo attano dukkhamanānupuṭṭho,",
"ja478:10.2": "Pavedaye jantu akālarūpe;",
"ja478:10.3": "Ānandino tassa bhavantimittā,",
"ja478:10.4": "Hitesino tassa dukhī bhavanti.",
"ja478:11.1": "Kālañca ñatvāna tathāvidhassa,",
"ja478:11.2": "Medhāvīnaṃ ekamanaṃ viditvā;",
"ja478:11.3": "Akkheyya tibbāni parassa dhīro,",
"ja478:11.4": "Saṇhaṃ giraṃ atthavatiṃ pamuñce.",
"ja478:12.1": "Sace ca jaññā avisayhamattano,",
"ja478:12.2": "Na te hi mayhaṃ sukhāgamāya;",
"ja478:12.3": "Ekova tibbāni saheyya dhīro,",
"ja478:12.4": "Saccaṃ hirottappamapekkhamāno.",
"ja478:13.1": "Ahaṃ raṭṭhe vicaranto,",
"ja478:13.2": "nigame rājadhāniyo;",
"ja478:13.3": "Bhikkhamāno mahārāja,",
"ja478:13.4": "ācariyassa dhanatthiko.",
"ja478:14.1": "Gahapatī rājapurise,",
"ja478:14.2": "mahāsāle ca brāhmaṇe;",
"ja478:14.3": "Alatthaṃ satta nikkhāni,",
"ja478:14.4": "suvaṇṇassa janādhipa;",
"ja478:14.5": "Te me naṭṭhā mahārāja,",
"ja478:14.6": "tasmā socāmahaṃ bhusaṃ.",
"ja478:15.1": "Purisā te mahārāja,",
"ja478:15.2": "manasānuvicintitā;",
"ja478:15.3": "Nālaṃ dukkhā pamocetuṃ,",
"ja478:15.4": "tasmā tesaṃ na byāhariṃ.",
"ja478:16.1": "Tvañca kho me mahārāja,",
"ja478:16.2": "manasānuvicintito;",
"ja478:16.3": "Alaṃ dukkhā pamocetuṃ,",
"ja478:16.4": "tasmā tuyhaṃ pavedayiṃ”.",
"ja478:17.1": "“Tassādāsi pasannatto,",
"ja478:17.2": "kāsīnaṃ raṭṭhavaḍḍhano;",
"ja478:17.3": "Jātarūpamaye nikkhe,",
"ja478:17.4": "suvaṇṇassa catuddasā”ti.",
"ja478:18.1": "Dūtajātakaṃ pañcamaṃ."
}