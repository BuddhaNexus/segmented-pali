{
"ja28:1.1": "Jātaka",
"ja28:2.1": "Ekakanipāta",
"ja28:3.1": "Kuruṅgavagga",
"ja28:4.1": "8. Nandivisālajātaka",
"ja28:5.1": "“Manuññameva bhāseyya,",
"ja28:5.2": "nāmanuññaṃ kudācanaṃ;",
"ja28:5.3": "Manuññaṃ bhāsamānassa,",
"ja28:5.4": "garuṃ bhāraṃ udaddhari;",
"ja28:5.5": "Dhanañca naṃ alābhesi,",
"ja28:5.6": "tena cattamano ahū”ti.",
"ja28:6.1": "Nandivisālajātakaṃ aṭṭhamaṃ."
}