{
  "ja28:0.1": "Jātaka ",
  "ja28:0.2": "Ekakanipāta ",
  "ja28:0.3": "Kuruṅgavagga ",
  "ja28:0.4": "8. Nandivisālajātaka ",
  "ja28:1.1": "“Manuññameva bhāseyya, ",
  "ja28:1.2": "nāmanuññaṁ kudācanaṁ; ",
  "ja28:1.3": "Manuññaṁ bhāsamānassa, ",
  "ja28:1.4": "garuṁ bhāraṁ udaddhari; ",
  "ja28:1.5": "Dhanañca naṁ alābhesi, ",
  "ja28:1.6": "tena cattamano ahū”ti. ",
  "ja28:2.1": "Nandivisālajātakaṁ aṭṭhamaṁ. "
}