{
"ja181:1.1": "Jātaka",
"ja181:2.1": "Dukanipāta",
"ja181:3.1": "Asadisavagga",
"ja181:4.1": "1. Asadisajātaka",
"ja181:5.1": "Dhanuggaho asadiso,",
"ja181:5.2": "rājaputto mahabbalo;",
"ja181:5.3": "Dūrepātī akkhaṇavedhī,",
"ja181:5.4": "mahākāyappadālano.",
"ja181:6.1": "Sabbāmitte raṇaṃ katvā,",
"ja181:6.2": "na ca kañci viheṭhayi;",
"ja181:6.3": "Bhātaraṃ sotthiṃ katvāna,",
"ja181:6.4": "saṃyamaṃ ajjhupāgamīti.",
"ja181:7.1": "Asadisajātakaṃ paṭhamaṃ."
}