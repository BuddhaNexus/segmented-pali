{
  "ja181:0.1": "Jātaka ",
  "ja181:0.2": "Dukanipāta ",
  "ja181:0.3": "Asadisavagga ",
  "ja181:0.4": "1. Asadisajātaka ",
  "ja181:1.1": "Dhanuggaho asadiso, ",
  "ja181:1.2": "rājaputto mahabbalo; ",
  "ja181:1.3": "Dūrepātī akkhaṇavedhī, ",
  "ja181:1.4": "mahākāyappadālano. ",
  "ja181:2.1": "Sabbāmitte raṇaṁ katvā, ",
  "ja181:2.2": "na ca kañci viheṭhayi; ",
  "ja181:2.3": "Bhātaraṁ sotthiṁ katvāna, ",
  "ja181:2.4": "saṁyamaṁ ajjhupāgamīti. ",
  "ja181:3.1": "Asadisajātakaṁ paṭhamaṁ. "
}