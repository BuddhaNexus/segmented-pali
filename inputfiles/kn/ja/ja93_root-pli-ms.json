{
"ja93:1.1": "Jātaka",
"ja93:2.1": "Ekakanipāta",
"ja93:3.1": "Littavagga",
"ja93:4.1": "3. Visāsabhojanajātaka",
"ja93:5.1": "“Na vissase avissatthe,",
"ja93:5.2": "vissatthepi na vissase;",
"ja93:5.3": "Vissāsā bhayamanveti,",
"ja93:5.4": "sīhaṃva migamātukāti.",
"ja93:6.1": "Visāsabhojanajātakaṃ tatiyaṃ."
}