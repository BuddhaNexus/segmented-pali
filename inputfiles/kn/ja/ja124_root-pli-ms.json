{
"ja124:1.1": "Jātaka",
"ja124:2.1": "Ekakanipāta",
"ja124:3.1": "Kusanāḷivagga",
"ja124:4.1": "4. Ambajātaka",
"ja124:5.1": "“Vāyametheva puriso,",
"ja124:5.2": "na nibbindeyya paṇḍito;",
"ja124:5.3": "Vāyāmassa phalaṃ passa,",
"ja124:5.4": "bhuttā ambā anītihan”ti.",
"ja124:6.1": "Ambajātakaṃ catutthaṃ."
}