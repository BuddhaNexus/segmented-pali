{
"ja149:1.1": "Jātaka",
"ja149:2.1": "Ekakanipāta",
"ja149:3.1": "Kakaṇṭakavagga",
"ja149:4.1": "9. Ekapaṇṇajātaka",
"ja149:5.1": "“Ekapaṇṇo ayaṃ rukkho,",
"ja149:5.2": "na bhūmyā caturaṅgulo;",
"ja149:5.3": "Phalena visakappena,",
"ja149:5.4": "mahāyaṃ kiṃ bhavissatī”ti.",
"ja149:6.1": "Ekapaṇṇajātakaṃ navamaṃ."
}