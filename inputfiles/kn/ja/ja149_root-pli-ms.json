{
  "ja149:0.1": "Jātaka ",
  "ja149:0.2": "Ekakanipāta ",
  "ja149:0.3": "Kakaṇṭakavagga ",
  "ja149:0.4": "9. Ekapaṇṇajātaka ",
  "ja149:1.1": "“Ekapaṇṇo ayaṁ rukkho, ",
  "ja149:1.2": "na bhūmyā caturaṅgulo; ",
  "ja149:1.3": "Phalena visakappena, ",
  "ja149:1.4": "mahāyaṁ kiṁ bhavissatī”ti. ",
  "ja149:2.1": "Ekapaṇṇajātakaṁ navamaṁ. "
}