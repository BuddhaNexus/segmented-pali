{
"ja290:1.1": "Jātaka",
"ja290:2.1": "Tikanipāta",
"ja290:3.1": "Abbhantaravagga",
"ja290:4.1": "10. Sīlavīmaṃsakajātaka",
"ja290:5.1": "“Sīlaṃ kireva kalyāṇaṃ,",
"ja290:5.2": "sīlaṃ loke anuttaraṃ;",
"ja290:5.3": "Passa ghoraviso nāgo,",
"ja290:5.4": "sīlavāti na haññati.",
"ja290:6.1": "Sohaṃ sīlaṃ samādissaṃ,",
"ja290:6.2": "loke anumataṃ sivaṃ;",
"ja290:6.3": "Ariyavuttisamācāro,",
"ja290:6.4": "yena vuccati sīlavā.",
"ja290:7.1": "Ñātīnañca piyo hoti,",
"ja290:7.2": "mittesu ca virocati;",
"ja290:7.3": "Kāyassa bhedā sugatiṃ,",
"ja290:7.4": "upapajjati sīlavā”ti.",
"ja290:8.1": "Sīlavīmaṃsakajātakaṃ dasamaṃ.",
"ja290:8.2": "Abbhantaravaggo catuttho.",
"ja290:9.1": "Tassuddānaṃ",
"ja290:10.1": "Duma kaṃsavaruttamabyagghamigā,",
"ja290:10.2": "Maṇayo maṇi sālukamavhayano;",
"ja290:10.3": "Anusāsaniyopi ca macchavaro,",
"ja290:10.4": "Maṇikuṇḍalakena kirena dasāti."
}