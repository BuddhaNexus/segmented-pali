{
"ja326:1.1": "Jātaka",
"ja326:2.1": "Catukkanipāta",
"ja326:3.1": "Kuṭidūsakavagga",
"ja326:4.1": "6. Kakkārujātaka",
"ja326:5.1": "“Kāyena yo nāvahare,",
"ja326:5.2": "vācāya na musā bhaṇe;",
"ja326:5.3": "Yaso laddhā na majjeyya,",
"ja326:5.4": "sa ve kakkārumarahati”.",
"ja326:6.1": "“Dhammena vittameseyya,",
"ja326:6.2": "na nikatyā dhanaṃ hare;",
"ja326:6.3": "Bhoge laddhā na majjeyya,",
"ja326:6.4": "sa ve kakkārumarahati”.",
"ja326:7.1": "“Yassa cittaṃ ahāliddaṃ,",
"ja326:7.2": "saddhā ca avirāginī;",
"ja326:7.3": "Eko sāduṃ na bhuñjeyya,",
"ja326:7.4": "sa ve kakkārumarahati”.",
"ja326:8.1": "“Sammukhā vā tirokkhā vā,",
"ja326:8.2": "Yo sante na paribhāsati;",
"ja326:8.3": "Yathāvādī tathākārī,",
"ja326:8.4": "Sa ve kakkārumarahatī”ti.",
"ja326:9.1": "Kakkārujātakaṃ chaṭṭhaṃ."
}