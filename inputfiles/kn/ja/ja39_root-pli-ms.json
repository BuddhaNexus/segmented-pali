{
  "ja39:0.1": "Jātaka ",
  "ja39:0.2": "Ekakanipāta ",
  "ja39:0.3": "Kulāvakavagga ",
  "ja39:0.4": "9. Nandajātaka ",
  "ja39:1.1": "“Maññe sovaṇṇayo rāsi, ",
  "ja39:1.2": "soṇṇamālā ca nandako; ",
  "ja39:1.3": "Yattha dāso āmajāto, ",
  "ja39:1.4": "ṭhito thullāni gajjatī”ti. ",
  "ja39:2.1": "Nandajātakaṁ navamaṁ. "
}