{
"ja39:1.1": "Jātaka",
"ja39:2.1": "Ekakanipāta",
"ja39:3.1": "Kulāvakavagga",
"ja39:4.1": "9. Nandajātaka",
"ja39:5.1": "“Maññe sovaṇṇayo rāsi,",
"ja39:5.2": "soṇṇamālā ca nandako;",
"ja39:5.3": "Yattha dāso āmajāto,",
"ja39:5.4": "ṭhito thullāni gajjatī”ti.",
"ja39:6.1": "Nandajātakaṃ navamaṃ."
}