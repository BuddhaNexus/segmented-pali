{
"ja201:1.1": "Jātaka",
"ja201:2.1": "Dukanipāta",
"ja201:3.1": "Nataṃdaḷhavagga",
"ja201:4.1": "1. Bandhanāgārajātaka",
"ja201:5.1": "“Na taṃ daḷhaṃ bandhanamāhu dhīrā,",
"ja201:5.2": "Yadāyasaṃ dārujapabbajañca;",
"ja201:5.3": "Sārattarattā maṇikuṇḍalesu,",
"ja201:5.4": "Puttesu dāresu ca yā apekkhā.",
"ja201:6.1": "Etaṃ daḷhaṃ bandhanamāhu dhīrā,",
"ja201:6.2": "Ohārinaṃ sīthilaṃ duppamuñcaṃ;",
"ja201:6.3": "Etampi chetvāna vajanti dhīrā,",
"ja201:6.4": "Anapekkhino kāmasukhaṃ pahāyā”ti.",
"ja201:7.1": "Bandhanāgārajātakaṃ paṭhamaṃ."
}