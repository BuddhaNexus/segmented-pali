{
"ja15:1.1": "Jātaka",
"ja15:2.1": "Ekakanipāta",
"ja15:3.1": "Sīlavagga",
"ja15:4.1": "5. Kharādiyajātaka",
"ja15:5.1": "“Aṭṭhakkhuraṃ kharādiye,",
"ja15:5.2": "migaṃ vaṅkātivaṅkinaṃ;",
"ja15:5.3": "Sattahi kālātikkantaṃ,",
"ja15:5.4": "na naṃ ovaditussahe”ti.",
"ja15:6.1": "Kharādiyajātakaṃ pañcamaṃ."
}