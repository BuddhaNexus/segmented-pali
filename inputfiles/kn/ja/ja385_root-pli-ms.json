{
"ja385:1.1": "Jātaka",
"ja385:2.1": "Chakkanipāta",
"ja385:3.1": "Avāriyavagga",
"ja385:4.1": "10. Nandiyamigarājajātaka",
"ja385:5.1": "“Sace brāhmaṇa gacchesi,",
"ja385:5.2": "sākete ajjunaṃ vanaṃ;",
"ja385:5.3": "Vajjāsi nandiyaṃ nāma,",
"ja385:5.4": "puttaṃ asmākamorasaṃ;",
"ja385:5.5": "Mātā pitā ca te vuddhā,",
"ja385:5.6": "te taṃ icchanti passituṃ”.",
"ja385:6.1": "“Bhuttā mayā nivāpāni,",
"ja385:6.2": "Rājino pānabhojanaṃ;",
"ja385:6.3": "Taṃ rājapiṇḍaṃ avabhottuṃ,",
"ja385:6.4": "Nāhaṃ brāhmaṇa mussahe”.",
"ja385:7.1": "“Odahissāmahaṃ passaṃ,",
"ja385:7.2": "khurappānissa rājino;",
"ja385:7.3": "Tadāhaṃ sukhito mutto,",
"ja385:7.4": "api passeyya mātaraṃ.",
"ja385:8.1": "Migarājā pure āsiṃ,",
"ja385:8.2": "kosalassa niketane;",
"ja385:8.3": "Nandiyo nāma nāmena,",
"ja385:8.4": "abhirūpo catuppado.",
"ja385:9.1": "Taṃ maṃ vadhitumāgacchi,",
"ja385:9.2": "dāyasmiṃ ajjune vane;",
"ja385:9.3": "Dhanuṃ advejjhaṃ katvāna,",
"ja385:9.4": "usuṃ sannayha kosalo.",
"ja385:10.1": "Tassāhaṃ odahiṃ passaṃ,",
"ja385:10.2": "khurappānissa rājino;",
"ja385:10.3": "Tadāhaṃ sukhito mutto,",
"ja385:10.4": "mātaraṃ daṭṭhumāgato”ti.",
"ja385:11.1": "Nandiyamigarājajātakaṃ dasamaṃ.",
"ja385:11.2": "Avāriyavaggo paṭhamo.",
"ja385:12.1": "Tassuddānaṃ",
"ja385:13.1": "Atha kujjharathesabha ketuvaro,",
"ja385:13.2": "Sadarīmukha neru latā ca puna;",
"ja385:13.3": "Apananda sirī ca sucittavaro,",
"ja385:13.4": "Atha dhammika nandimigena dasāti."
}