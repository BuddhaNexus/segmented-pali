{
  "ja385:0.1": "Jātaka ",
  "ja385:0.2": "Chakkanipāta ",
  "ja385:0.3": "Avāriyavagga ",
  "ja385:0.4": "10. Nandiyamigarājajātaka ",
  "ja385:1.1": "“Sace brāhmaṇa gacchesi, ",
  "ja385:1.2": "sākete ajjunaṁ vanaṁ; ",
  "ja385:1.3": "Vajjāsi nandiyaṁ nāma, ",
  "ja385:1.4": "puttaṁ asmākamorasaṁ; ",
  "ja385:1.5": "Mātā pitā ca te vuddhā, ",
  "ja385:1.6": "te taṁ icchanti passituṁ”. ",
  "ja385:2.1": "“Bhuttā mayā nivāpāni, ",
  "ja385:2.2": "Rājino pānabhojanaṁ; ",
  "ja385:2.3": "Taṁ rājapiṇḍaṁ avabhottuṁ, ",
  "ja385:2.4": "Nāhaṁ brāhmaṇa mussahe”. ",
  "ja385:3.1": "“Odahissāmahaṁ passaṁ, ",
  "ja385:3.2": "khurappānissa rājino; ",
  "ja385:3.3": "Tadāhaṁ sukhito mutto, ",
  "ja385:3.4": "api passeyya mātaraṁ. ",
  "ja385:4.1": "Migarājā pure āsiṁ, ",
  "ja385:4.2": "kosalassa niketane; ",
  "ja385:4.3": "Nandiyo nāma nāmena, ",
  "ja385:4.4": "abhirūpo catuppado. ",
  "ja385:5.1": "Taṁ maṁ vadhitumāgacchi, ",
  "ja385:5.2": "dāyasmiṁ ajjune vane; ",
  "ja385:5.3": "Dhanuṁ advejjhaṁ katvāna, ",
  "ja385:5.4": "usuṁ sannayha kosalo. ",
  "ja385:6.1": "Tassāhaṁ odahiṁ passaṁ, ",
  "ja385:6.2": "khurappānissa rājino; ",
  "ja385:6.3": "Tadāhaṁ sukhito mutto, ",
  "ja385:6.4": "mātaraṁ daṭṭhumāgato”ti. ",
  "ja385:7.1": "Nandiyamigarājajātakaṁ dasamaṁ. ",
  "ja385:7.2": "Avāriyavaggo paṭhamo. ",
  "ja385:8.1": "Tassuddānaṁ ",
  "ja385:9.1": "Atha kujjharathesabha ketuvaro, ",
  "ja385:9.2": "Sadarīmukha neru latā ca puna; ",
  "ja385:9.3": "Apananda sirī ca sucittavaro, ",
  "ja385:9.4": "Atha dhammika nandimigena dasāti. "
}