{
  "ja462:0.1": "Jātaka ",
  "ja462:0.2": "Ekādasakanipāta ",
  "ja462:0.3": "Mātuposakavagga ",
  "ja462:0.4": "8. Saṁvarajātaka ",
  "ja462:1.1": "“Jānanto no mahārāja, ",
  "ja462:1.2": "tava sīlaṁ janādhipo; ",
  "ja462:1.3": "Ime kumāre pūjento, ",
  "ja462:1.4": "na taṁ kenaci maññatha. ",
  "ja462:2.1": "Tiṭṭhante no mahārāje, ",
  "ja462:2.2": "adu deve divaṅgate; ",
  "ja462:2.3": "Ñātī taṁ samanuññiṁsu, ",
  "ja462:2.4": "sampassaṁ atthamattano. ",
  "ja462:3.1": "Kena saṁvara vattena, ",
  "ja462:3.2": "sañjāte abhitiṭṭhasi; ",
  "ja462:3.3": "Kena taṁ nātivattanti, ",
  "ja462:3.4": "ñātisaṅghā samāgatā”. ",
  "ja462:4.1": "“Na rājaputta usūyāmi, ",
  "ja462:4.2": "samaṇānaṁ mahesinaṁ; ",
  "ja462:4.3": "Sakkaccaṁ te namassāmi, ",
  "ja462:4.4": "pāde vandāmi tādinaṁ. ",
  "ja462:5.1": "Te maṁ dhammaguṇe yuttaṁ, ",
  "ja462:5.2": "sussūsamanusūyakaṁ; ",
  "ja462:5.3": "Samaṇā manusāsanti, ",
  "ja462:5.4": "isī dhammaguṇe ratā. ",
  "ja462:6.1": "Tesāhaṁ vacanaṁ sutvā, ",
  "ja462:6.2": "samaṇānaṁ mahesinaṁ; ",
  "ja462:6.3": "Na kiñci atimaññāmi, ",
  "ja462:6.4": "dhamme me nirato mano. ",
  "ja462:7.1": "Hatthārohā anīkaṭṭhā, ",
  "ja462:7.2": "rathikā pattikārakā; ",
  "ja462:7.3": "Tesaṁ nappaṭibandhāmi, ",
  "ja462:7.4": "niviṭṭhaṁ bhattavetanaṁ. ",
  "ja462:8.1": "Mahāmattā ca me atthi, ",
  "ja462:8.2": "mantino paricārakā; ",
  "ja462:8.3": "Bārāṇasiṁ voharanti, ",
  "ja462:8.4": "bahumaṁsasurodakaṁ. ",
  "ja462:9.1": "Athopi vāṇijā phītā, ",
  "ja462:9.2": "nānāraṭṭhehi āgatā; ",
  "ja462:9.3": "Tesu me vihitā rakkhā, ",
  "ja462:9.4": "evaṁ jānāhuposatha”. ",
  "ja462:10.1": "“Dhammena kira ñātīnaṁ, ",
  "ja462:10.2": "rajjaṁ kārehi saṁvara; ",
  "ja462:10.3": "Medhāvī paṇḍito cāsi, ",
  "ja462:10.4": "athopi ñātinaṁ hito. ",
  "ja462:11.1": "Taṁ taṁ ñātiparibyūḷhaṁ, ",
  "ja462:11.2": "nānāratanamocitaṁ; ",
  "ja462:11.3": "Amittā nappasahanti, ",
  "ja462:11.4": "indaṁva asurādhipo”ti. ",
  "ja462:12.1": "Saṁvarajātakaṁ aṭṭhamaṁ. "
}