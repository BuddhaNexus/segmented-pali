{
"ja395:1.1": "Jātaka",
"ja395:2.1": "Chakkanipāta",
"ja395:3.1": "Kharaputtavagga",
"ja395:4.1": "10. Pārāvatajātaka",
"ja395:5.1": "“Cirassaṃ vata passāmi,",
"ja395:5.2": "sahāyaṃ maṇidhārinaṃ;",
"ja395:5.3": "Sukatā massukuttiyā,",
"ja395:5.4": "sobhate vata me sakhā”.",
"ja395:6.1": "“Parūḷhakacchanakhalomo,",
"ja395:6.2": "ahaṃ kammesu byāvaṭo;",
"ja395:6.3": "Cirassaṃ nhāpitaṃ laddhā,",
"ja395:6.4": "lomaṃ taṃ ajja hārayiṃ”.",
"ja395:7.1": "“Yannu lomaṃ ahāresi,",
"ja395:7.2": "dullabhaṃ laddha kappakaṃ;",
"ja395:7.3": "Atha kiñcarahi te samma,",
"ja395:7.4": "kaṇṭhe kiṇikiṇāyati”.",
"ja395:8.1": "“Manussasukhumālānaṃ,",
"ja395:8.2": "maṇi kaṇṭhesu lambati;",
"ja395:8.3": "Tesāhaṃ anusikkhāmi,",
"ja395:8.4": "mā tvaṃ maññi davā kataṃ.",
"ja395:9.1": "Sacepimaṃ pihayasi,",
"ja395:9.2": "massukuttiṃ sukāritaṃ;",
"ja395:9.3": "Kārayissāmi te samma,",
"ja395:9.4": "maṇiñcāpi dadāmi te”.",
"ja395:10.1": "“Tvaññeva maṇinā channo,",
"ja395:10.2": "Sukatāya ca massuyā;",
"ja395:10.3": "Āmanta kho taṃ gacchāmi,",
"ja395:10.4": "Piyaṃ me tavadassanan”ti. #980",
"ja395:11.1": "Pārāvatajātakaṃ dasamaṃ.",
"ja395:11.2": "Kharaputtavaggo dutiyo.",
"ja395:12.1": "Tassuddānaṃ",
"ja395:13.1": "Atha passa sasūci ca tuṇḍilako,",
"ja395:13.2": "Miga mayhakapañcamapakkhivaro;",
"ja395:13.3": "Atha pañjali vārija mejjha puna,",
"ja395:13.4": "Atha vaṭṭa kapotavarena dasāti.",
"ja395:14.1": "Atha vagguddānaṃ",
"ja395:15.1": "Atha vaggaṃ pakittissaṃ,",
"ja395:15.2": "chanipātaṃ varuttame;",
"ja395:15.3": "Avāriyā ca kharo ca,",
"ja395:15.4": "dve ca vuttā subyañjanāti.",
"ja395:16.1": "Chakkanipātaṃ niṭṭhitaṃ."
}