{
  "ja391:0.1": "Jātaka ",
  "ja391:0.2": "Chakkanipāta ",
  "ja391:0.3": "Kharaputtavagga ",
  "ja391:0.4": "6. Vijjādharajātaka ",
  "ja391:1.1": "“Dubbaṇṇarūpaṁ tuvamariyavaṇṇī, ",
  "ja391:1.2": "Purakkhatvā pañjaliko namassasi; ",
  "ja391:1.3": "Seyyo nu te so udavā sarikkho, ",
  "ja391:1.4": "Nāmaṁ parassattano cāpi brūhi”. ",
  "ja391:2.1": "“Na nāmagottaṁ gaṇhanti rāja, ",
  "ja391:2.2": "Sammaggatānujjugatāna devā; ",
  "ja391:2.3": "Ahañca te nāmadheyyaṁ vadāmi, ",
  "ja391:2.4": "Sakkohamasmī tidasānamindo”. ",
  "ja391:3.1": "“Yo disvā bhikkhuṁ caraṇūpapannaṁ, ",
  "ja391:3.2": "Purakkhatvā pañjaliko namassati; ",
  "ja391:3.3": "Pucchāmi taṁ devarājetamatthaṁ, ",
  "ja391:3.4": "Ito cuto kiṁ labhate sukhaṁ so”. ",
  "ja391:4.1": "“Yo disvā bhikkhuṁ caraṇūpapannaṁ, ",
  "ja391:4.2": "Purakkhatvā pañjaliko namassati; ",
  "ja391:4.3": "Diṭṭheva dhamme labhate pasaṁsaṁ, ",
  "ja391:4.4": "Saggañca so yāti sarīrabhedā”. ",
  "ja391:5.1": "“Lakkhī vata me udapādi ajja, ",
  "ja391:5.2": "Yaṁ vāsavaṁ bhūtapatiddasāma; ",
  "ja391:5.3": "Bhikkhuñca disvāna tuvañca sakka, ",
  "ja391:5.4": "Kāhāmi puññāni anappakāni”. ",
  "ja391:6.1": "“Addhā have sevitabbā sapaññā, ",
  "ja391:6.2": "Bahussutā ye bahuṭhānacintino; ",
  "ja391:6.3": "Bhikkhuñca disvāna mamañca rāja, ",
  "ja391:6.4": "Karohi puññāni anappakāni”. ",
  "ja391:7.1": "“Akkodhano niccapasannacitto, ",
  "ja391:7.2": "Sabbātithīyācayogo bhavitvā; ",
  "ja391:7.3": "Nihacca mānaṁ abhivādayissaṁ, ",
  "ja391:7.4": "Sutvāna devinda subhāsitānī”ti. ",
  "ja391:8.1": "Vijjādharajātakaṁ chaṭṭhaṁ. "
}