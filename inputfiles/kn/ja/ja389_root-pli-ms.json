{
"ja389:1.1": "Jātaka",
"ja389:2.1": "Chakkanipāta",
"ja389:3.1": "Kharaputtavagga",
"ja389:4.1": "4. Suvaṇṇakakkaṭajātaka",
"ja389:5.1": "“Siṅgīmigo āyatacakkhunetto,",
"ja389:5.2": "Aṭṭhittaco vārisayo alomo;",
"ja389:5.3": "Tenābhibhūto kapaṇaṃ rudāmi,",
"ja389:5.4": "Hare sakhā kissa nu maṃ jahāsi”.",
"ja389:6.1": "So passasanto mahatā phaṇena,",
"ja389:6.2": "Bhujaṅgamo kakkaṭamajjhapatto;",
"ja389:6.3": "Sakhā sakhāraṃ paritāyamāno,",
"ja389:6.4": "Bhujaṅgamaṃ kakkaṭako gahesi.",
"ja389:7.1": "“Na vāyasaṃ no pana kaṇhasappaṃ,",
"ja389:7.2": "Ghāsatthiko kakkaṭako adeyya;",
"ja389:7.3": "Pucchāmi taṃ āyatacakkhunetta,",
"ja389:7.4": "Atha kissa hetumha ubho gahītā”.",
"ja389:8.1": "“Ayaṃ puriso mama atthakāmo,",
"ja389:8.2": "Yo maṃ gahetvāna dakāya neti;",
"ja389:8.3": "Tasmiṃ mate dukkhamanappakaṃ me,",
"ja389:8.4": "Ahañca eso ca ubho na homa.",
"ja389:9.1": "Mamañca disvāna pavaddhakāyaṃ,",
"ja389:9.2": "Sabbo jano hiṃsitumeva micche;",
"ja389:9.3": "Sāduñca thūlañca muduñca maṃsaṃ,",
"ja389:9.4": "Kākāpi maṃ disvāna viheṭhayeyyuṃ”.",
"ja389:10.1": "“Sacetassa hetumha ubho gahītā,",
"ja389:10.2": "Uṭṭhātu poso visamāvamāmi;",
"ja389:10.3": "Mamañca kākañca pamuñca khippaṃ,",
"ja389:10.4": "Pure visaṃ gāḷhamupeti maccaṃ”.",
"ja389:11.1": "“Sappaṃ pamokkhāmi na tāva kākaṃ,",
"ja389:11.2": "Paṭibandhako hohiti tāva kāko;",
"ja389:11.3": "Purisañca disvāna sukhiṃ arogaṃ,",
"ja389:11.4": "Kākaṃ pamokkhāmi yatheva sappaṃ”.",
"ja389:12.1": "“Kāko tadā devadatto ahosi,",
"ja389:12.2": "Māro pana kaṇhasappo ahosi;",
"ja389:12.3": "Ānandabhaddo kakkaṭako ahosi,",
"ja389:12.4": "Ahaṃ tadā brāhmaṇo homi satthā”ti.",
"ja389:13.1": "Suvaṇṇakakkaṭajātakaṃ catutthaṃ."
}