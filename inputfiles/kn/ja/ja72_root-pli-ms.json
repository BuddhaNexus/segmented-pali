{
"ja72:1.1": "Jātaka",
"ja72:2.1": "Ekakanipāta",
"ja72:3.1": "Varuṇavagga",
"ja72:4.1": "2. Sīlavahatthijātaka",
"ja72:5.1": "“Akataññussa posassa,",
"ja72:5.2": "niccaṃ vivaradassino;",
"ja72:5.3": "Sabbañce pathaviṃ dajjā,",
"ja72:5.4": "neva naṃ abhirādhaye”ti.",
"ja72:6.1": "Sīlavahatthijātakaṃ dutiyaṃ."
}