{
"ja242:1.1": "Jātaka",
"ja242:2.1": "Dukanipāta",
"ja242:3.1": "Siṅgālavagga",
"ja242:4.1": "2. Sunakhajātaka",
"ja242:5.1": "“Bālo vatāyaṃ sunakho,",
"ja242:5.2": "yo varattaṃ na khādati;",
"ja242:5.3": "Bandhanā ca pamuñceyya,",
"ja242:5.4": "asito ca gharaṃ vaje”.",
"ja242:6.1": "“Aṭṭhitaṃ me manasmiṃ me,",
"ja242:6.2": "atho me hadaye kataṃ;",
"ja242:6.3": "Kālañca paṭikaṅkhāmi,",
"ja242:6.4": "yāva passupatū jano”ti.",
"ja242:7.1": "Sunakhajātakaṃ dutiyaṃ."
}