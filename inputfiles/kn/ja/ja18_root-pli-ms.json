{
"ja18:1.1": "Jātaka",
"ja18:2.1": "Ekakanipāta",
"ja18:3.1": "Sīlavagga",
"ja18:4.1": "8. Matakabhattajātaka",
"ja18:5.1": "“Evañce sattā jāneyyuṃ,",
"ja18:5.2": "dukkhāyaṃ jātisambhavo;",
"ja18:5.3": "Na pāṇo pāṇinaṃ haññe,",
"ja18:5.4": "pāṇaghātī hi socatī”ti.",
"ja18:6.1": "Matakabhattajātakaṃ aṭṭhamaṃ."
}