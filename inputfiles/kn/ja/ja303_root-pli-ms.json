{
  "ja303:0.1": "Jātaka ",
  "ja303:0.2": "Catukkanipāta ",
  "ja303:0.3": "Kāliṅgavagga ",
  "ja303:0.4": "3. Ekarājajātaka ",
  "ja303:1.1": "“Anuttare kāmaguṇe samiddhe, ",
  "ja303:1.2": "Bhutvāna pubbe vasī ekarāja; ",
  "ja303:1.3": "So dāni dugge narakamhi khitto, ",
  "ja303:1.4": "Nappajjahe vaṇṇabalaṁ purāṇaṁ”. ",
  "ja303:2.1": "“Pubbeva khantī ca tapo ca mayhaṁ, ",
  "ja303:2.2": "Sampatthitā dubbhisena ahosi; ",
  "ja303:2.3": "Taṁ dāni laddhāna kathaṁ nu rāja, ",
  "ja303:2.4": "Jahe ahaṁ vaṇṇabalaṁ purāṇaṁ. ",
  "ja303:3.1": "Sabbā kirevaṁ pariniṭṭhitāni, ",
  "ja303:3.2": "Yasassinaṁ paññavantaṁ visayha; ",
  "ja303:3.3": "Yaso ca laddhā purimaṁ uḷāraṁ, ",
  "ja303:3.4": "Nappajjahe vaṇṇabalaṁ purāṇaṁ. ",
  "ja303:4.1": "Panujja dukkhena sukhaṁ janinda, ",
  "ja303:4.2": "Sukhena vā dukkhamasayhasāhi; ",
  "ja303:4.3": "Ubhayattha santo abhinibbutattā, ",
  "ja303:4.4": "Sukhe ca dukkhe ca bhavanti tulyā”ti. ",
  "ja303:5.1": "Ekarājajātakaṁ tatiyaṁ. "
}