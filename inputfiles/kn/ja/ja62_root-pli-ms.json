{
"ja62:1.1": "Jātaka",
"ja62:2.1": "Ekakanipāta",
"ja62:3.1": "Itthivagga",
"ja62:4.1": "2. Aṇḍabhūtajātaka",
"ja62:5.1": "“Yaṃ brāhmaṇo avādesi,",
"ja62:5.2": "vīṇaṃ samukhaveṭhito;",
"ja62:5.3": "Aṇḍabhūtā bhatā bhariyā,",
"ja62:5.4": "tāsu ko jātu vissase”ti.",
"ja62:6.1": "Aṇḍabhūtajātakaṃ dutiyaṃ."
}