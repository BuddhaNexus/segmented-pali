{
  "ja104:0.1": "Jātaka ",
  "ja104:0.2": "Ekakanipāta ",
  "ja104:0.3": "Parosatavagga ",
  "ja104:0.4": "4. Mittavindakajātaka ",
  "ja104:1.1": "“Catubbhi aṭṭhajjhagamā, ",
  "ja104:1.2": "aṭṭhāhipi ca soḷasa; ",
  "ja104:1.3": "Soḷasāhi ca bāttiṁsa, ",
  "ja104:1.4": "atricchaṁ cakkamāsado; ",
  "ja104:1.5": "Icchāhatassa posassa, ",
  "ja104:1.6": "cakkaṁ bhamati matthake”ti. ",
  "ja104:2.1": "Mittavindakajātakaṁ catutthaṁ. "
}