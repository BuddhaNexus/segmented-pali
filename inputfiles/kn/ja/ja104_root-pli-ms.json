{
"ja104:1.1": "Jātaka",
"ja104:2.1": "Ekakanipāta",
"ja104:3.1": "Parosatavagga",
"ja104:4.1": "4. Mittavindakajātaka",
"ja104:5.1": "“Catubbhi aṭṭhajjhagamā,",
"ja104:5.2": "aṭṭhāhipi ca soḷasa;",
"ja104:5.3": "Soḷasāhi ca bāttiṃsa,",
"ja104:5.4": "atricchaṃ cakkamāsado;",
"ja104:5.5": "Icchāhatassa posassa,",
"ja104:5.6": "cakkaṃ bhamati matthake”ti.",
"ja104:6.1": "Mittavindakajātakaṃ catutthaṃ."
}