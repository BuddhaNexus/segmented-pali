{
"ja1:1.1": "Jātaka",
"ja1:2.1": "Ekakanipāta",
"ja1:3.1": "Apaṇṇakavagga",
"ja1:4.1": "1. Apaṇṇakajātaka",
"ja1:5.1": "Namo tassa Bhagavato Arahato Sammāsambuddhassa.",
"ja1:6.1": "“Apaṇṇakaṃ ṭhānameke,",
"ja1:6.2": "Dutiyaṃ āhu takkikā;",
"ja1:6.3": "Etadaññāya medhāvī,",
"ja1:6.4": "Taṃ gaṇhe yadapaṇṇakan”ti.",
"ja1:7.1": "Apaṇṇakajātakaṃ paṭhamaṃ."
}