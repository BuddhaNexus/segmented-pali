{
  "ja423:0.1": "Jātaka ",
  "ja423:0.2": "Aṭṭhakanipāta ",
  "ja423:0.3": "Kaccānivagga ",
  "ja423:0.4": "7. Indriyajātaka ",
  "ja423:1.1": "“Yo indriyānaṁ kāmena, ",
  "ja423:1.2": "vasaṁ nārada gacchati; ",
  "ja423:1.3": "So pariccajjubho loke, ",
  "ja423:1.4": "jīvantova visussati. ",
  "ja423:2.1": "Sukhassānantaraṁ dukkhaṁ, ",
  "ja423:2.2": "dukkhassānantaraṁ sukhaṁ; ",
  "ja423:2.3": "Sosi patto sukhā dukkhaṁ, ",
  "ja423:2.4": "pāṭikaṅkha varaṁ sukhaṁ. ",
  "ja423:3.1": "Kicchakāle kicchasaho, ",
  "ja423:3.2": "yo kicchaṁ nātivattati; ",
  "ja423:3.3": "Sa kicchantaṁ sukhaṁ dhīro, ",
  "ja423:3.4": "yogaṁ samadhigacchati. ",
  "ja423:4.1": "Na heva kāmāna kāmā, ",
  "ja423:4.2": "nānatthā nātthakāraṇā; ",
  "ja423:4.3": "Na katañca niraṅkatvā, ",
  "ja423:4.4": "dhammā cavitumarahasi”. ",
  "ja423:5.1": "“Dakkhaṁ gahapatī sādhu, ",
  "ja423:5.2": "saṁvibhajjañca bhojanaṁ; ",
  "ja423:5.3": "Ahāso atthalābhesu, ",
  "ja423:5.4": "atthabyāpatti abyatho”. ",
  "ja423:6.1": "“Ettāvatetaṁ paṇḍiccaṁ, ",
  "ja423:6.2": "api so devilo bravi; ",
  "ja423:6.3": "Na yito kiñci pāpiyo, ",
  "ja423:6.4": "yo indriyānaṁ vasaṁ vaje”. ",
  "ja423:7.1": "“Amittānaṁva hatthatthaṁ, ",
  "ja423:7.2": "sivi pappoti māmiva; ",
  "ja423:7.3": "Kammaṁ vijjañca dakkheyyaṁ, ",
  "ja423:7.4": "vivāhaṁ sīlamaddavaṁ; ",
  "ja423:7.5": "Ete ca yase hāpetvā, ",
  "ja423:7.6": "nibbatto sehi kammehi. ",
  "ja423:8.1": "Sohaṁ sahassajīnova, ",
  "ja423:8.2": "abandhu aparāyaṇo; ",
  "ja423:8.3": "Ariyadhammā apakkanto, ",
  "ja423:8.4": "yathā peto tathevahaṁ. ",
  "ja423:9.1": "Sukhakāme dukkhāpetvā, ",
  "ja423:9.2": "āpannosmi padaṁ imaṁ; ",
  "ja423:9.3": "So sukhaṁ nādhigacchāmi, ",
  "ja423:9.4": "ṭhito bhāṇumatāmivā”ti. ",
  "ja423:10.1": "Indriyajātakaṁ sattamaṁ. "
}