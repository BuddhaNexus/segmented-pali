{
"ja81:1.1": "Jātaka",
"ja81:2.1": "Ekakanipāta",
"ja81:3.1": "Apāyimhavagga",
"ja81:4.1": "1. Surāpānajātaka",
"ja81:5.1": "“Apāyimha anaccimha,",
"ja81:5.2": "agāyimha rudimha ca;",
"ja81:5.3": "Visaññikaraṇiṃ pitvā,",
"ja81:5.4": "diṭṭhā nāhumha vānarā”ti.",
"ja81:6.1": "Surāpānajātakaṃ paṭhamaṃ."
}