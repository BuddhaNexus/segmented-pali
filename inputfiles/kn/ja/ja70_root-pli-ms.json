{
"ja70:1.1": "Jātaka",
"ja70:2.1": "Ekakanipāta",
"ja70:3.1": "Itthivagga",
"ja70:4.1": "10. Kuddālajātaka",
"ja70:5.1": "“Na taṃ jitaṃ sādhu jitaṃ,",
"ja70:5.2": "yaṃ jitaṃ avajīyati;",
"ja70:5.3": "Taṃ kho jitaṃ sādhu jitaṃ,",
"ja70:5.4": "yaṃ jitaṃ nāvajīyatī”ti.",
"ja70:6.1": "Kuddālajātakaṃ dasamaṃ.",
"ja70:6.2": "Itthivaggo sattamo.",
"ja70:7.1": "Tassuddānaṃ",
"ja70:8.1": "Sikhīsabbaghasopi ca vīṇavaro,",
"ja70:8.2": "Pisuṇā mittabhedikā nandī nadī;",
"ja70:8.3": "Mudulakkhaṇa sodariyā ca mano,",
"ja70:8.4": "Visa sādhujitena bhavanti dasāti."
}