{
"ja180:1.1": "Jātaka",
"ja180:2.1": "Dukanipāta",
"ja180:3.1": "Kalyāṇavagga",
"ja180:4.1": "10. Duddadajātaka",
"ja180:5.1": "“Duddadaṃ dadamānānaṃ,",
"ja180:5.2": "dukkaraṃ kamma kubbataṃ;",
"ja180:5.3": "Asanto nānukubbanti,",
"ja180:5.4": "sataṃ dhammo durannayo”.",
"ja180:6.1": "“Tasmā satañca asataṃ,",
"ja180:6.2": "nānā hoti ito gati;",
"ja180:6.3": "Asanto nirayaṃ yanti,",
"ja180:6.4": "santo saggaparāyaṇāti.",
"ja180:7.1": "Duddadajātakaṃ dasamaṃ.",
"ja180:7.2": "Kalyāṇavaggo tatiyo.",
"ja180:8.1": "Tassuddānaṃ",
"ja180:9.1": "Susamaññamigādhibhū māṇavako,",
"ja180:9.2": "Vāripahūtarūpādiccupaṭṭhānā;",
"ja180:9.3": "Sakaḷāyasatindukapaṅka puna,",
"ja180:9.4": "Satadhamma sududdadakena dasā”ti."
}