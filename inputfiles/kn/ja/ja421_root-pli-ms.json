{
  "ja421:0.1": "Jātaka ",
  "ja421:0.2": "Aṭṭhakanipāta ",
  "ja421:0.3": "Kaccānivagga ",
  "ja421:0.4": "5. Gaṅgamālajātaka ",
  "ja421:1.1": "“Aṅgārajātā pathavī, ",
  "ja421:1.2": "kukkuḷānugatā mahī; ",
  "ja421:1.3": "Atha gāyasi vattāni, ",
  "ja421:1.4": "na taṁ tapati ātapo. ",
  "ja421:2.1": "Uddhaṁ tapati ādicco, ",
  "ja421:2.2": "adho tapati vālukā; ",
  "ja421:2.3": "Atha gāyasi vattāni, ",
  "ja421:2.4": "na taṁ tapati ātapo”. ",
  "ja421:3.1": "“Na maṁ tapati ātapo, ",
  "ja421:3.2": "ātapā tapayanti maṁ; ",
  "ja421:3.3": "Atthā hi vividhā rāja, ",
  "ja421:3.4": "te tapanti na ātapo”. ",
  "ja421:4.1": "“Addasaṁ kāma te mūlaṁ, ",
  "ja421:4.2": "saṅkappā kāma jāyasi; ",
  "ja421:4.3": "Na taṁ saṅkappayissāmi, ",
  "ja421:4.4": "evaṁ kāma na hehisi”. ",
  "ja421:5.1": "“Appāpi kāmā na alaṁ, ",
  "ja421:5.2": "bahūhipi na tappati; ",
  "ja421:5.3": "Ahahā bālalapanā, ",
  "ja421:5.4": "parivajjetha jaggato”. ",
  "ja421:6.1": "“Appassa kammassa phalaṁ mamedaṁ, ",
  "ja421:6.2": "Udayo ajjhāgamā mahattapattaṁ; ",
  "ja421:6.3": "Suladdhalābho vata māṇavassa, ",
  "ja421:6.4": "Yo pabbajī kāmarāgaṁ pahāya”. ",
  "ja421:7.1": "“Tapasā pajahanti pāpakammaṁ, ",
  "ja421:7.2": "Tapasā nhāpitakumbhakārabhāvaṁ; ",
  "ja421:7.3": "Tapasā abhibhuyya gaṅgamāla, ",
  "ja421:7.4": "Nāmenālapasajja brahmadattaṁ”. ",
  "ja421:8.1": "“Sandiṭṭhikameva ‘amma’ passatha, ",
  "ja421:8.2": "Khantīsoraccassa ayaṁ vipāko; ",
  "ja421:8.3": "Yo sabbajanassa vanditohu, ",
  "ja421:8.4": "Taṁ vandāma sarājikā samaccā”. ",
  "ja421:9.1": "“Mā kiñci avacuttha gaṅgamālaṁ, ",
  "ja421:9.2": "Muninaṁ monapathesu sikkhamānaṁ; ",
  "ja421:9.3": "Eso hi atari aṇṇavaṁ, ",
  "ja421:9.4": "Yaṁ taritvā caranti vītasokā”ti. ",
  "ja421:10.1": "Gaṅgamālajātakaṁ pañcamaṁ. "
}