{
  "ja212:0.1": "Jātaka ",
  "ja212:0.2": "Dukanipāta ",
  "ja212:0.3": "Bīraṇathambhavagga ",
  "ja212:0.4": "2. Ucchiṭṭhabhattajātaka ",
  "ja212:1.1": "“Añño uparimo vaṇṇo, ",
  "ja212:1.2": "añño vaṇṇo ca heṭṭhimo; ",
  "ja212:1.3": "Brāhmaṇī tveva pucchāmi, ",
  "ja212:1.4": "kiṁ heṭṭhā kiñca uppari”. ",
  "ja212:2.1": "“Ahaṁ naṭosmi bhaddante, ",
  "ja212:2.2": "bhikkhakosmi idhāgato; ",
  "ja212:2.3": "Ayañhi koṭṭhamotiṇṇo, ",
  "ja212:2.4": "ayaṁ so yaṁ gavesasī”ti. ",
  "ja212:3.1": "Ucchiṭṭhabhattajātakaṁ dutiyaṁ. "
}