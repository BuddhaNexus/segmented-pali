{
  "ja332:0.1": "Jātaka ",
  "ja332:0.2": "Catukkanipāta ",
  "ja332:0.3": "Kokilavagga ",
  "ja332:0.4": "2. Rathalaṭṭhijātaka ",
  "ja332:1.1": "“Api hantvā hato brūti, ",
  "ja332:1.2": "jetvā jitoti bhāsati; ",
  "ja332:1.3": "Pubbamakkhāyino rāja, ",
  "ja332:1.4": "aññadatthu na saddahe. ",
  "ja332:2.1": "Tasmā paṇḍitajātiyo, ",
  "ja332:2.2": "suṇeyya itarassapi; ",
  "ja332:2.3": "Ubhinnaṁ vacanaṁ sutvā, ",
  "ja332:2.4": "yathā dhammo tathā kare. ",
  "ja332:3.1": "Alaso gihī kāmabhogī na sādhu, ",
  "ja332:3.2": "Asaññato pabbajito na sādhu; ",
  "ja332:3.3": "Rājā na sādhu anisammakārī, ",
  "ja332:3.4": "Yo paṇḍito kodhano taṁ na sādhu. ",
  "ja332:4.1": "Nisamma khattiyo kayirā, ",
  "ja332:4.2": "nānisamma disampati; ",
  "ja332:4.3": "Nisammakārino rāja, ",
  "ja332:4.4": "yaso kitti ca vaḍḍhatī”ti. ",
  "ja332:5.1": "Rathalaṭṭhijātakaṁ dutiyaṁ. "
}