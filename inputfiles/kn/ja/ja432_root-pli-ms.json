{
  "ja432:0.1": "Jātaka ",
  "ja432:0.2": "Navakanipāta ",
  "ja432:0.3": "Gijjhavagga ",
  "ja432:0.4": "6. Padakusalamāṇavajātaka ",
  "ja432:1.1": "“Bahussutaṁ cittakathiṁ, ",
  "ja432:1.2": "gaṅgā vahati pāṭaliṁ; ",
  "ja432:1.3": "Vuyhamānaka bhaddante, ",
  "ja432:1.4": "ekaṁ me dehi gāthakaṁ”. ",
  "ja432:2.1": "“Yena siñcanti dukkhitaṁ, ",
  "ja432:2.2": "yena siñcanti āturaṁ; ",
  "ja432:2.3": "Tassa majjhe marissāmi, ",
  "ja432:2.4": "jātaṁ saraṇato bhayaṁ”. ",
  "ja432:3.1": "“Yattha bījāni rūhanti, ",
  "ja432:3.2": "sattā yattha patiṭṭhitā; ",
  "ja432:3.3": "Sā me sīsaṁ nipīḷeti, ",
  "ja432:3.4": "jātaṁ saraṇato bhayaṁ”. ",
  "ja432:4.1": "“Yena bhattāni paccanti, ",
  "ja432:4.2": "sītaṁ yena vihaññati; ",
  "ja432:4.3": "So maṁ ḍahati gattāni, ",
  "ja432:4.4": "jātaṁ saraṇato bhayaṁ”. ",
  "ja432:5.1": "“Yena bhuttena yāpenti, ",
  "ja432:5.2": "puthū brāhmaṇakhattiyā; ",
  "ja432:5.3": "So maṁ bhutto byāpādeti, ",
  "ja432:5.4": "jātaṁ saraṇato bhayaṁ”. ",
  "ja432:6.1": "“Gimhānaṁ pacchime māse, ",
  "ja432:6.2": "vātamicchanti paṇḍitā; ",
  "ja432:6.3": "So maṁ bhañjati gattāni, ",
  "ja432:6.4": "jātaṁ saraṇato bhayaṁ”. ",
  "ja432:7.1": "“Yaṁ nissitā jagatiruhaṁ, ",
  "ja432:7.2": "svāyaṁ aggiṁ pamuñcati; ",
  "ja432:7.3": "Disā bhajatha vakkaṅgā, ",
  "ja432:7.4": "jātaṁ saraṇato bhayaṁ”. ",
  "ja432:8.1": "“Yamānayiṁ somanassaṁ, ",
  "ja432:8.2": "māliniṁ candanussadaṁ; ",
  "ja432:8.3": "Sā maṁ gharā nicchubhati, ",
  "ja432:8.4": "jātaṁ saraṇato bhayaṁ”. ",
  "ja432:9.1": "“Yena jātena nandissaṁ, ",
  "ja432:9.2": "yassa ca bhavamicchisaṁ; ",
  "ja432:9.3": "So maṁ gharā nicchubhati, ",
  "ja432:9.4": "jātaṁ saraṇato bhayaṁ”. ",
  "ja432:10.1": "“Suṇantu me jānapadā, ",
  "ja432:10.2": "negamā ca samāgatā; ",
  "ja432:10.3": "Yatodakaṁ tadādittaṁ, ",
  "ja432:10.4": "yato khemaṁ tato bhayaṁ. ",
  "ja432:11.1": "Rājā vilumpate raṭṭhaṁ, ",
  "ja432:11.2": "brāhmaṇo ca purohito; ",
  "ja432:11.3": "Attaguttā viharatha, ",
  "ja432:11.4": "jātaṁ saraṇato bhayan”ti. ",
  "ja432:12.1": "Padakusalamāṇavajātakaṁ chaṭṭhaṁ. "
}