{
"ja373:1.1": "Jātaka",
"ja373:2.1": "Pañcakanipāta",
"ja373:3.1": "Aḍḍhavagga",
"ja373:4.1": "3. Mūsikajātaka",
"ja373:5.1": "“Kuhiṃ gatā kattha gatā,",
"ja373:5.2": "iti lālappatī jano;",
"ja373:5.3": "Ahameveko jānāmi,",
"ja373:5.4": "udapāne mūsikā hatā.",
"ja373:6.1": "Yañcetaṃ iti cīti ca,",
"ja373:6.2": "gadrabhova nivattasi;",
"ja373:6.3": "Udapāne mūsikaṃ hantvā,",
"ja373:6.4": "yavaṃ bhakkhetumicchasi.",
"ja373:7.1": "Daharo cāsi dummedha,",
"ja373:7.2": "paṭhamuppattiko susu;",
"ja373:7.3": "Dīghañcetaṃ samāsajja,",
"ja373:7.4": "na te dassāmi jīvitaṃ.",
"ja373:8.1": "Nāntalikkhabhavanena,",
"ja373:8.2": "nāṅgaputtapinena vā;",
"ja373:8.3": "Puttena hi patthayito,",
"ja373:8.4": "silokehi pamocito.",
"ja373:9.1": "Sabbaṃ sutamadhīyetha,",
"ja373:9.2": "hīnamukkaṭṭhamajjhimaṃ;",
"ja373:9.3": "Sabbassa atthaṃ jāneyya,",
"ja373:9.4": "na ca sabbaṃ payojaye;",
"ja373:9.5": "Hoti tādisako kālo,",
"ja373:9.6": "yattha atthāvahaṃ sutan”ti.",
"ja373:10.1": "Mūsikajātakaṃ tatiyaṃ."
}