{
"ja355:1.1": "Jātaka",
"ja355:2.1": "Pañcakanipāta",
"ja355:3.1": "Maṇikuṇḍalavagga",
"ja355:4.1": "5. Ghaṭajātaka",
"ja355:5.1": "“Aññe socanti rodanti,",
"ja355:5.2": "aññe assumukhā janā;",
"ja355:5.3": "Pasannamukhavaṇṇosi,",
"ja355:5.4": "kasmā ghaṭa na socasi”.",
"ja355:6.1": "“Nābbhatītaharo soko,",
"ja355:6.2": "nānāgatasukhāvaho;",
"ja355:6.3": "Tasmā dhaṅka na socāmi,",
"ja355:6.4": "natthi soke dutīyatā.",
"ja355:7.1": "Socaṃ paṇḍu kiso hoti,",
"ja355:7.2": "bhattañcassa na ruccati;",
"ja355:7.3": "Amittā sumanā honti,",
"ja355:7.4": "sallaviddhassa ruppato.",
"ja355:8.1": "Gāme vā yadi vāraññe,",
"ja355:8.2": "ninne vā yadi vā thale;",
"ja355:8.3": "Ṭhitaṃ maṃ nāgamissati,",
"ja355:8.4": "evaṃ diṭṭhapado ahaṃ.",
"ja355:9.1": "Yassattā nālamekova,",
"ja355:9.2": "sabbakāmarasāharo;",
"ja355:9.3": "Sabbāpi pathavī tassa,",
"ja355:9.4": "na sukhaṃ āvahissatī”ti.",
"ja355:10.1": "Ghaṭajātakaṃ pañcamaṃ."
}