{
"ja57:1.1": "Jātaka",
"ja57:2.1": "Ekakanipāta",
"ja57:3.1": "Āsīsavagga",
"ja57:4.1": "7. Vānarindajātaka",
"ja57:5.1": "“Yassete caturo dhammā,",
"ja57:5.2": "vānarinda yathā tava;",
"ja57:5.3": "Saccaṃ dhammo dhiti cāgo,",
"ja57:5.4": "diṭṭhaṃ so ativattatī”ti.",
"ja57:6.1": "Vānarindajātakaṃ sattamaṃ."
}