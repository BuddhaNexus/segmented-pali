{
"ja243:1.1": "Jātaka",
"ja243:2.1": "Dukanipāta",
"ja243:3.1": "Siṅgālavagga",
"ja243:4.1": "3. Guttilajātaka",
"ja243:5.1": "“Sattatantiṃ sumadhuraṃ,",
"ja243:5.2": "rāmaṇeyyaṃ avācayiṃ;",
"ja243:5.3": "So maṃ raṅgamhi avheti,",
"ja243:5.4": "saraṇaṃ me hohi kosiya”.",
"ja243:6.1": "“Ahaṃ taṃ saraṇaṃ samma,",
"ja243:6.2": "Ahamācariyapūjako;",
"ja243:6.3": "Na taṃ jayissati sisso,",
"ja243:6.4": "Sissamācariya jessasī”ti.",
"ja243:7.1": "Guttilajātakaṃ tatiyaṃ."
}