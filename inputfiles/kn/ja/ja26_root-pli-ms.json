{
"ja26:1.1": "Jātaka",
"ja26:2.1": "Ekakanipāta",
"ja26:3.1": "Kuruṅgavagga",
"ja26:4.1": "6. Mahiḷāmukhajātaka",
"ja26:5.1": "“Purāṇacorāna vaco nisamma,",
"ja26:5.2": "Mahiḷāmukho pothayamanvacārī;",
"ja26:5.3": "Susaññatānañhi vaco nisamma,",
"ja26:5.4": "Gajuttamo sabbaguṇesu aṭṭhā”ti.",
"ja26:6.1": "Mahiḷāmukhajātakaṃ chaṭṭhaṃ."
}