{
"ja250:1.1": "Jātaka",
"ja250:2.1": "Dukanipāta",
"ja250:3.1": "Siṅgālavagga",
"ja250:4.1": "10. Kapijātaka",
"ja250:5.1": "“Ayaṃ isī upasamasaṃyame rato,",
"ja250:5.2": "Sa tiṭṭhati sisirabhayena aṭṭito;",
"ja250:5.3": "Handa ayaṃ pavisatumaṃ agārakaṃ,",
"ja250:5.4": "Vinetu sītaṃ darathañca kevalaṃ”.",
"ja250:6.1": "“Nāyaṃ isī upasamasaṃyame rato,",
"ja250:6.2": "Kapī ayaṃ dumavarasākhagocaro;",
"ja250:6.3": "So dūsako rosako cāpi jammo,",
"ja250:6.4": "Sacevajemampi dūseyyagāran”ti. #351",
"ja250:7.1": "Kapijātakaṃ dasamaṃ.",
"ja250:7.2": "Siṅgālavaggo dasamo.",
"ja250:8.1": "Tassuddānaṃ",
"ja250:9.1": "Atha rājā siṅgālavaro sunakho,",
"ja250:9.2": "Tathā kosiya icchati kālaghaso;",
"ja250:9.3": "Atha dānavaroṭṭhapi sārathinā,",
"ja250:9.4": "Punambavanañca sisirakapi dasāti.",
"ja250:10.1": "Atha vagguddānaṃ",
"ja250:11.1": "Daḷhañca vaggaṃ aparena santhavaṃ,",
"ja250:11.2": "Kalyāṇavaggāsadiso ca rūhakaṃ;",
"ja250:11.3": "Nataṃdaḷha bīraṇathambhakaṃ puna,",
"ja250:11.4": "Kāsāvupāhana siṅgālakena dasāti.",
"ja250:12.1": "Dukanipātaṃ niṭṭhitaṃ."
}