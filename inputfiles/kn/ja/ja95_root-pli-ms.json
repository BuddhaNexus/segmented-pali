{
"ja95:1.1": "Jātaka",
"ja95:2.1": "Ekakanipāta",
"ja95:3.1": "Littavagga",
"ja95:4.1": "5. Mahāsudassanajātaka",
"ja95:5.1": "“Aniccā vata saṅkhārā,",
"ja95:5.2": "uppādavayadhammino;",
"ja95:5.3": "Uppajjitvā nirujjhanti,",
"ja95:5.4": "tesaṃ vūpasamo sukho”ti.",
"ja95:6.1": "Mahāsudassanajātakaṃ pañcamaṃ."
}