{
  "ja492:0.1": "Jātaka ",
  "ja492:0.2": "Pakiṇṇakanipāta ",
  "ja492:0.3": "Sālikedāravagga ",
  "ja492:0.4": "9. Tacchasūkarajātaka ",
  "ja492:1.1": "“Yadesamānā vicarimha, ",
  "ja492:1.2": "pabbatāni vanāni ca; ",
  "ja492:1.3": "Anvesaṁ vicariṁ ñātī, ",
  "ja492:1.4": "teme adhigatā mayā. ",
  "ja492:2.1": "Bahuñcidaṁ mūlaphalaṁ, ",
  "ja492:2.2": "bhakkho cāyaṁ anappako; ",
  "ja492:2.3": "Rammā cimā girīnajjo, ",
  "ja492:2.4": "phāsuvāso bhavissati. ",
  "ja492:3.1": "Idhevāhaṁ vasissāmi, ",
  "ja492:3.2": "saha sabbehi ñātibhi; ",
  "ja492:3.3": "Appossukko nirāsaṅkī, ",
  "ja492:3.4": "asoko akutobhayo”. ",
  "ja492:4.1": "“Aññampi leṇaṁ pariyesa, ",
  "ja492:4.2": "sattu no idha vijjati; ",
  "ja492:4.3": "So taccha sūkare hanti, ",
  "ja492:4.4": "idhāgantvā varaṁ varaṁ”. ",
  "ja492:5.1": "“Ko numhākaṁ idha sattu, ",
  "ja492:5.2": "ko ñātī susamāgate; ",
  "ja492:5.3": "Duppadhaṁse padhaṁseti, ",
  "ja492:5.4": "taṁ me akkhātha pucchitā”. ",
  "ja492:6.1": "“Uddhaggarājī migarājā, ",
  "ja492:6.2": "balī dāṭhāvudho migo; ",
  "ja492:6.3": "So taccha sūkare hanti, ",
  "ja492:6.4": "idhāgantvā varaṁ varaṁ”. ",
  "ja492:7.1": "“Na no dāṭhā na vijjanti, ",
  "ja492:7.2": "balaṁ kāye samohitaṁ; ",
  "ja492:7.3": "Sabbe samaggā hutvāna, ",
  "ja492:7.4": "vasaṁ kāhāma ekakaṁ”. ",
  "ja492:8.1": "“Hadayaṅgamaṁ kaṇṇasukhaṁ, ",
  "ja492:8.2": "Vācaṁ bhāsasi tacchaka; ",
  "ja492:8.3": "Yopi yuddhe palāyeyya, ",
  "ja492:8.4": "Tampi pacchā hanāmase”. ",
  "ja492:9.1": "“Pāṇātipātā virato nu ajja, ",
  "ja492:9.2": "Abhayaṁ nu te sabbabhūtesu dinnaṁ; ",
  "ja492:9.3": "Dāṭhā nu te migavadhāya na santi, ",
  "ja492:9.4": "Yo saṅghapatto kapaṇova jhāyasi”. ",
  "ja492:10.1": "“Na me dāṭhā na vijjanti, ",
  "ja492:10.2": "Balaṁ kāye samohitaṁ; ",
  "ja492:10.3": "Ñātī ca disvāna sāmaggī ekato, ",
  "ja492:10.4": "Tasmā ca jhāyāmi vanamhi ekako. ",
  "ja492:11.1": "Imassudaṁ yanti disodisaṁ pure, ",
  "ja492:11.2": "Bhayaṭṭitā leṇagavesino puthu; ",
  "ja492:11.3": "Te dāni saṅgamma vasanti ekato, ",
  "ja492:11.4": "Yatthaṭṭhitā duppasahajja te mayā. ",
  "ja492:12.1": "Pariṇāyakasampannā, ",
  "ja492:12.2": "sahitā ekavādino; ",
  "ja492:12.3": "Te maṁ samaggā hiṁseyyuṁ, ",
  "ja492:12.4": "tasmā nesaṁ na patthaye”. ",
  "ja492:13.1": "“Ekova indo asure jināti, ",
  "ja492:13.2": "Ekova seno hanti dije pasayha; ",
  "ja492:13.3": "Ekova byaggho migasaṅghapatto, ",
  "ja492:13.4": "Varaṁ varaṁ hanti balañhi tādisaṁ”. ",
  "ja492:14.1": "“Na heva indo na seno, ",
  "ja492:14.2": "napi byaggho migādhipo; ",
  "ja492:14.3": "Samagge sahite ñātī, ",
  "ja492:14.4": "na byagghe kurute vase”. ",
  "ja492:15.1": "“Kumbhīlakā sakuṇakā, ",
  "ja492:15.2": "saṅghino gaṇacārino; ",
  "ja492:15.3": "Sammodamānā ekajjhaṁ, ",
  "ja492:15.4": "uppatanti ḍayanti ca. ",
  "ja492:16.1": "Tesañca ḍayamānānaṁ, ",
  "ja492:16.2": "ekettha apasakkati; ",
  "ja492:16.3": "Tañca seno nitāḷeti, ",
  "ja492:16.4": "veyyagghiyeva sā gati”. ",
  "ja492:17.1": "“Ussāhito jaṭilena, ",
  "ja492:17.2": "luddenāmisacakkhunā; ",
  "ja492:17.3": "Dāṭhī dāṭhīsu pakkhandi, ",
  "ja492:17.4": "maññamāno yathā pure”. ",
  "ja492:18.1": "“Sādhu sambahulā ñātī, ",
  "ja492:18.2": "api rukkhā araññajā; ",
  "ja492:18.3": "Sūkarehi samaggehi, ",
  "ja492:18.4": "byaggho ekāyane hato”. ",
  "ja492:19.1": "Brāhmaṇañceva byagghañca, ",
  "ja492:19.2": "ubho hantvāna sūkarā. ",
  "ja492:19.3": "Ānandino pamuditā, ",
  "ja492:19.4": "mahānādaṁ panādisuṁ. ",
  "ja492:20.1": "Te su udumbaramūlasmiṁ, ",
  "ja492:20.2": "sūkarā susamāgatā; ",
  "ja492:20.3": "Tacchakaṁ abhisiñciṁsu, ",
  "ja492:20.4": "“tvaṁ no rājāsi issaro”ti. ",
  "ja492:21.1": "Tacchasūkarajātakaṁ navamaṁ. "
}