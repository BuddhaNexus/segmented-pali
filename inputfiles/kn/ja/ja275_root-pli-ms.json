{
"ja275:1.1": "Jātaka",
"ja275:2.1": "Tikanipāta",
"ja275:3.1": "Udapānavagga",
"ja275:4.1": "5. Rucirajātaka",
"ja275:5.1": "“Kāyaṃ balākā rucirā,",
"ja275:5.2": "kākanīḷasmimacchati;",
"ja275:5.3": "Caṇḍo kāko sakhā mayhaṃ,",
"ja275:5.4": "yassa cetaṃ kulāvakaṃ”.",
"ja275:6.1": "“Nanu maṃ samma jānāsi,",
"ja275:6.2": "dija sāmākabhojana;",
"ja275:6.3": "Akatvā vacanaṃ tuyhaṃ,",
"ja275:6.4": "passa lūnosmi āgato”.",
"ja275:7.1": "“Punapāpajjasī samma,",
"ja275:7.2": "sīlañhi tava tādisaṃ;",
"ja275:7.3": "Na hi mānusakā bhogā,",
"ja275:7.4": "subhuñjā honti pakkhinā”ti.",
"ja275:8.1": "Rucirajātakaṃ pañcamaṃ."
}