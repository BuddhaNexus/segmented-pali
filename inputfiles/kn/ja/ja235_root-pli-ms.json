{
"ja235:1.1": "Jātaka",
"ja235:2.1": "Dukanipāta",
"ja235:3.1": "Upāhanavagga",
"ja235:4.1": "5. Vacchanakhajātaka",
"ja235:5.1": "“Sukhā gharā vacchanakha,",
"ja235:5.2": "sahiraññā sabhojanā;",
"ja235:5.3": "Yattha bhutvā pivitvā ca,",
"ja235:5.4": "sayeyyātha anussuko”.",
"ja235:6.1": "“Gharā nānīhamānassa,",
"ja235:6.2": "Gharā nābhaṇato musā;",
"ja235:6.3": "Gharā nādinnadaṇḍassa,",
"ja235:6.4": "Paresaṃ anikubbato;",
"ja235:6.5": "Evaṃ chiddaṃ durabhisambhavaṃ,",
"ja235:6.6": "Ko gharaṃ paṭipajjatī”ti.",
"ja235:7.1": "Vacchanakhajātakaṃ pañcamaṃ."
}