{
"ja153:1.1": "Jātaka",
"ja153:2.1": "Dukanipāta",
"ja153:3.1": "Daḷhavagga",
"ja153:4.1": "3. Sūkarajātaka",
"ja153:5.1": "“Catuppado ahaṃ samma,",
"ja153:5.2": "tvampi samma catuppado;",
"ja153:5.3": "Ehi samma nivattassu,",
"ja153:5.4": "kiṃ nu bhīto palāyasi”.",
"ja153:6.1": "“Asuci pūtilomosi,",
"ja153:6.2": "duggandho vāsi sūkara;",
"ja153:6.3": "Sace yujjhitukāmosi,",
"ja153:6.4": "jayaṃ samma dadāmi te”ti.",
"ja153:7.1": "Sūkarajātakaṃ tatiyaṃ."
}