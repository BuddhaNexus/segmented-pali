{
"ja174:1.1": "Jātaka",
"ja174:2.1": "Dukanipāta",
"ja174:3.1": "Kalyāṇavagga",
"ja174:4.1": "4. Dubbhiyamakkaṭajātaka",
"ja174:5.1": "“Adamha te vāri pahūtarūpaṃ,",
"ja174:5.2": "Ghammābhitattassa pipāsitassa;",
"ja174:5.3": "So dāni pitvāna kiriṅkarosi,",
"ja174:5.4": "Asaṅgamo pāpajanena seyyo”.",
"ja174:6.1": "“Ko te suto vā diṭṭho vā,",
"ja174:6.2": "sīlavā nāma makkaṭo;",
"ja174:6.3": "Idāni kho taṃ ohacchaṃ,",
"ja174:6.4": "esā asmāka dhammatā”ti.",
"ja174:7.1": "Dubbhiyamakkaṭajātakaṃ catutthaṃ."
}