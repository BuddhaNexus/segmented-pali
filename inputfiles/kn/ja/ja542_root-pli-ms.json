{
  "ja542:0.1": "Jātaka ",
  "ja542:0.2": "Mahānipāta ",
  "ja542:0.3": "Mūgapakkhavagga ",
  "ja542:0.4": "5 Umaṅgajātaka ",
  "ja542:1.1": "“Pañcālo sabbasenāya, ",
  "ja542:1.2": "brahmadattoyamāgato; ",
  "ja542:1.3": "Sāyaṁ pañcāliyā senā, ",
  "ja542:1.4": "appameyyā mahosadha. ",
  "ja542:2.1": "Vīthimatī pattimatī, ",
  "ja542:2.2": "sabbasaṅgāmakovidā; ",
  "ja542:2.3": "Ohārinī saddavatī, ",
  "ja542:2.4": "bherisaṅkhappabodhanā. ",
  "ja542:3.1": "Lohavijjālaṅkārābhā, ",
  "ja542:3.2": "dhajinī vāmarohinī; ",
  "ja542:3.3": "Sippiyehi susampannā, ",
  "ja542:3.4": "sūrehi suppatiṭṭhitā. ",
  "ja542:4.1": "Dasettha paṇḍitā āhu, ",
  "ja542:4.2": "bhūripaññā rahogamā; ",
  "ja542:4.3": "Mātā ekādasī rañño, ",
  "ja542:4.4": "pañcāliyaṁ pasāsati. ",
  "ja542:5.1": "Athetthekasataṁ khatyā, ",
  "ja542:5.2": "anuyantā yasassino; ",
  "ja542:5.3": "Acchinnaraṭṭhā byathitā, ",
  "ja542:5.4": "pañcāliyaṁ vasaṁ gatā. ",
  "ja542:6.1": "Yaṁvadā takkarā rañño, ",
  "ja542:6.2": "akāmā piyabhāṇino; ",
  "ja542:6.3": "Pañcālamanuyāyanti, ",
  "ja542:6.4": "akāmā vasino gatā. ",
  "ja542:7.1": "Tāya senāya mithilā, ",
  "ja542:7.2": "tisandhiparivāritā; ",
  "ja542:7.3": "Rājadhānī videhānaṁ, ",
  "ja542:7.4": "samantā parikhaññati. ",
  "ja542:8.1": "Uddhaṁ tārakajātāva, ",
  "ja542:8.2": "samantā parivāritā; ",
  "ja542:8.3": "Mahosadha vijānāhi, ",
  "ja542:8.4": "kathaṁ mokkho bhavissati”. ",
  "ja542:9.1": "“Pāde deva pasārehi, ",
  "ja542:9.2": "bhuñja kāme ramassu ca; ",
  "ja542:9.3": "Hitvā pañcāliyaṁ senaṁ, ",
  "ja542:9.4": "brahmadatto palāyiti”. ",
  "ja542:10.1": "“Rājā santhavakāmo te, ",
  "ja542:10.2": "ratanāni pavecchati; ",
  "ja542:10.3": "Āgacchantu ito dūtā, ",
  "ja542:10.4": "mañjukā piyabhāṇino. ",
  "ja542:11.1": "Bhāsantu mudukā vācā, ",
  "ja542:11.2": "yā vācā paṭinanditā; ",
  "ja542:11.3": "Pañcālo ca videho ca, ",
  "ja542:11.4": "ubho ekā bhavantu te”. ",
  "ja542:12.1": "“Kathaṁ nu kevaṭṭa mahosadhena, ",
  "ja542:12.2": "Samāgamo āsi tadiṅgha brūhi; ",
  "ja542:12.3": "Kacci te paṭinijjhatto, ",
  "ja542:12.4": "Kacci tuṭṭho mahosadho”. ",
  "ja542:13.1": "“Anariyarūpo puriso janinda, ",
  "ja542:13.2": "Asammodako thaddho asabbhirūpo; ",
  "ja542:13.3": "Yathā mūgo ca badhiro ca, ",
  "ja542:13.4": "Na kiñcitthaṁ abhāsatha”. ",
  "ja542:14.1": "“Addhā idaṁ mantapadaṁ sududdasaṁ, ",
  "ja542:14.2": "Attho suddho naraviriyena diṭṭho; ",
  "ja542:14.3": "Tathā hi kāyo mama sampavedhati, ",
  "ja542:14.4": "Hitvā sayaṁ ko parahatthamessati”. ",
  "ja542:15.1": "“Channañhi ekāva matī sameti, ",
  "ja542:15.2": "Ye paṇḍitā uttamabhūripattā; ",
  "ja542:15.3": "Yānaṁ ayānaṁ atha vāpi ṭhānaṁ, ",
  "ja542:15.4": "Mahosadha tvampi matiṁ karohi”. ",
  "ja542:16.1": "“Jānāsi kho rāja mahānubhāvo, ",
  "ja542:16.2": "Mahabbalo cūḷanibrahmadatto; ",
  "ja542:16.3": "Rājā ca taṁ icchati māraṇatthaṁ, ",
  "ja542:16.4": "Migaṁ yathā okacarena luddo. ",
  "ja542:17.1": "Yathāpi maccho baḷisaṁ, ",
  "ja542:17.2": "vaṅkaṁ maṁsena chāditaṁ; ",
  "ja542:17.3": "Āmagiddho na jānāti, ",
  "ja542:17.4": "maccho maraṇamattano. ",
  "ja542:18.1": "Evameva tuvaṁ rāja, ",
  "ja542:18.2": "cūḷaneyyassa dhītaraṁ; ",
  "ja542:18.3": "Kāmagiddho na jānāsi, ",
  "ja542:18.4": "macchova maraṇamattano. ",
  "ja542:19.1": "Sace gacchasi pañcālaṁ, ",
  "ja542:19.2": "Khippamattaṁ jahissasi; ",
  "ja542:19.3": "Migaṁ panthānubandhaṁva, ",
  "ja542:19.4": "Mahantaṁ bhayamessati”. ",
  "ja542:20.1": "“Mayameva bālamhase eḷamūgā, ",
  "ja542:20.2": "Ye uttamatthāni tayī lapimhā; ",
  "ja542:20.3": "Kimeva tvaṁ naṅgalakoṭivaḍḍho, ",
  "ja542:20.4": "Atthāni jānāsi yathāpi aññe”. ",
  "ja542:21.1": "“Imaṁ gale gahetvāna, ",
  "ja542:21.2": "nāsetha vijitā mama; ",
  "ja542:21.3": "Yo me ratanalābhassa, ",
  "ja542:21.4": "antarāyāya bhāsati”. ",
  "ja542:22.1": "Tato ca so apakkamma, ",
  "ja542:22.2": "vedehassa upantikā; ",
  "ja542:22.3": "Atha āmantayī dūtaṁ, ",
  "ja542:22.4": "mādharaṁ suvapaṇḍitaṁ. ",
  "ja542:23.1": "“Ehi samma haritapakkha, ",
  "ja542:23.2": "veyyāvaccaṁ karohi me; ",
  "ja542:23.3": "Atthi pañcālarājassa, ",
  "ja542:23.4": "sāḷikā sayanapālikā. ",
  "ja542:24.1": "Taṁ bandhanena pucchassu, ",
  "ja542:24.2": "sā hi sabbassa kovidā; ",
  "ja542:24.3": "Sā tesaṁ sabbaṁ jānāti, ",
  "ja542:24.4": "rañño ca kosiyassa ca”. ",
  "ja542:25.1": "“Āmo”ti so paṭissutvā, ",
  "ja542:25.2": "mādharo suvapaṇḍito; ",
  "ja542:25.3": "Agamāsi haritapakkho, ",
  "ja542:25.4": "sāḷikāya upantikaṁ. ",
  "ja542:26.1": "Tato ca kho so gantvāna, ",
  "ja542:26.2": "mādharo suvapaṇḍito; ",
  "ja542:26.3": "Athāmantayi sugharaṁ, ",
  "ja542:26.4": "sāḷikaṁ mañjubhāṇikaṁ. ",
  "ja542:27.1": "“Kacci te sughare khamanīyaṁ, ",
  "ja542:27.2": "Kacci vesse anāmayaṁ; ",
  "ja542:27.3": "Kacci te madhunā lājā, ",
  "ja542:27.4": "Labbhate sughare tuvaṁ”. ",
  "ja542:28.1": "“Kusalañceva me samma, ",
  "ja542:28.2": "atho samma anāmayaṁ; ",
  "ja542:28.3": "Atho me madhunā lājā, ",
  "ja542:28.4": "labbhate suvapaṇḍita. ",
  "ja542:29.1": "Kuto nu samma āgamma, ",
  "ja542:29.2": "kassa vā pahito tuvaṁ; ",
  "ja542:29.3": "Na ca mesi ito pubbe, ",
  "ja542:29.4": "diṭṭho vā yadi vā suto”. ",
  "ja542:30.1": "“Ahosiṁ sivirājassa, ",
  "ja542:30.2": "pāsāde sayanapālako; ",
  "ja542:30.3": "Tato so dhammiko rājā, ",
  "ja542:30.4": "baddhe mocesi bandhanā. ",
  "ja542:31.1": "Tassa mekā dutiyāsi, ",
  "ja542:31.2": "sāḷikā mañjubhāṇikā; ",
  "ja542:31.3": "Taṁ tattha avadhī seno, ",
  "ja542:31.4": "pekkhato sughare mama. ",
  "ja542:32.1": "Tassā kāmā hi sammatto, ",
  "ja542:32.2": "āgatosmi tavantike; ",
  "ja542:32.3": "Sace kareyya okāsaṁ, ",
  "ja542:32.4": "ubhayova vasāmase”. ",
  "ja542:33.1": "“Suvova suviṁ kāmeyya, ",
  "ja542:33.2": "sāḷiko pana sāḷikaṁ; ",
  "ja542:33.3": "Suvassa sāḷikāyeva, ",
  "ja542:33.4": "saṁvāso hoti kīdiso”. ",
  "ja542:34.1": "“Yoyaṁ kāme kāmayati, ",
  "ja542:34.2": "api caṇḍālikāmapi; ",
  "ja542:34.3": "Sabbo hi sadiso hoti, ",
  "ja542:34.4": "natthi kāme asādiso”. ",
  "ja542:35.1": "“Atthi jampāvatī nāma, ",
  "ja542:35.2": "mātā sivissa rājino; ",
  "ja542:35.3": "Sā bhariyā vāsudevassa, ",
  "ja542:35.4": "kaṇhassa mahesī piyā. ",
  "ja542:36.1": "Raṭṭhavatī kimpurisī, ",
  "ja542:36.2": "sāpi vacchaṁ akāmayi; ",
  "ja542:36.3": "Manusso migiyā saddhiṁ, ",
  "ja542:36.4": "natthi kāme asādiso. ",
  "ja542:37.1": "Handa khvāhaṁ gamissāmi, ",
  "ja542:37.2": "sāḷike mañjubhāṇike; ",
  "ja542:37.3": "Paccakkhānupadañhetaṁ, ",
  "ja542:37.4": "atimaññasi nūna maṁ”. ",
  "ja542:38.1": "“Na sirī taramānassa, ",
  "ja542:38.2": "mādhara suvapaṇḍita; ",
  "ja542:38.3": "Idheva tāva acchassu, ",
  "ja542:38.4": "yāva rājāna dakkhasi; ",
  "ja542:38.5": "Sossi saddaṁ mudiṅgānaṁ, ",
  "ja542:38.6": "ānubhāvañca rājino”. ",
  "ja542:39.1": "“Yo nu khvāyaṁ tibbo saddo, ",
  "ja542:39.2": "tirojanapade suto; ",
  "ja542:39.3": "Dhītā pañcālarājassa, ",
  "ja542:39.4": "osadhī viya vaṇṇinī; ",
  "ja542:39.5": "Taṁ dassati videhānaṁ, ",
  "ja542:39.6": "so vivāho bhavissati”. ",
  "ja542:40.1": "“Ediso mā amittānaṁ, ",
  "ja542:40.2": "vivāho hotu mādhara; ",
  "ja542:40.3": "Yathā pañcālarājassa, ",
  "ja542:40.4": "vedehena bhavissati. ",
  "ja542:41.1": "Ānayitvāna vedehaṁ, ",
  "ja542:41.2": "pañcālānaṁ rathesabho; ",
  "ja542:41.3": "Tato naṁ ghātayissati, ",
  "ja542:41.4": "nassa sakhī bhavissati”. ",
  "ja542:42.1": "“Handa kho maṁ anujānāhi, ",
  "ja542:42.2": "rattiyo sattamattiyo; ",
  "ja542:42.3": "Yāvāhaṁ sivirājassa, ",
  "ja542:42.4": "ārocemi mahesino; ",
  "ja542:42.5": "Laddho ca me āvasatho, ",
  "ja542:42.6": "sāḷikāya upantikaṁ”. ",
  "ja542:43.1": "“Handa kho taṁ anujānāmi, ",
  "ja542:43.2": "rattiyo sattamattiyo; ",
  "ja542:43.3": "Sace tvaṁ sattarattena, ",
  "ja542:43.4": "nāgacchasi mamantike; ",
  "ja542:43.5": "Maññe okkantasattaṁ maṁ, ",
  "ja542:43.6": "matāya āgamissasi”. ",
  "ja542:44.1": "Tato ca kho so gantvāna, ",
  "ja542:44.2": "mādharo suvapaṇḍito; ",
  "ja542:44.3": "Mahosadhassa akkhāsi, ",
  "ja542:44.4": "sāḷikāvacanaṁ idaṁ. ",
  "ja542:45.1": "“Yasseva ghare bhuñjeyya bhogaṁ, ",
  "ja542:45.2": "Tasseva atthaṁ puriso careyya; ",
  "ja542:45.3": "Handāhaṁ gacchāmi pure janinda, ",
  "ja542:45.4": "Pañcālarājassa puraṁ surammaṁ; ",
  "ja542:45.5": "Nivesanāni māpetuṁ, ",
  "ja542:45.6": "Vedehassa yasassino. ",
  "ja542:46.1": "Nivesanāni māpetvā, ",
  "ja542:46.2": "vedehassa yasassino; ",
  "ja542:46.3": "Yadā te pahiṇeyyāmi, ",
  "ja542:46.4": "tadā eyyāsi khattiya”. ",
  "ja542:47.1": "Tato ca pāyāsi pure mahosadho, ",
  "ja542:47.2": "Pañcālarājassa puraṁ surammaṁ; ",
  "ja542:47.3": "Nivesanāni māpetuṁ, ",
  "ja542:47.4": "Vedehassa yasassino. ",
  "ja542:48.1": "Nivesanāni māpetvā, ",
  "ja542:48.2": "vedehassa yasassino; ",
  "ja542:48.3": "Athassa pāhiṇī dūtaṁ, ",
  "ja542:48.4": "vedehaṁ mithilaggahaṁ; ",
  "ja542:48.5": "“Ehi dāni mahārāja, ",
  "ja542:48.6": "māpitaṁ te nivesanaṁ”. ",
  "ja542:49.1": "Tato ca rājā pāyāsi, ",
  "ja542:49.2": "senāya caturaṅgiyā; ",
  "ja542:49.3": "Anantavāhanaṁ daṭṭhuṁ, ",
  "ja542:49.4": "phītaṁ kapiliyaṁ puraṁ. ",
  "ja542:50.1": "Tato ca kho so gantvāna, ",
  "ja542:50.2": "brahmadattassa pāhiṇi; ",
  "ja542:50.3": "“Āgatosmi mahārāja, ",
  "ja542:50.4": "tava pādāni vandituṁ. ",
  "ja542:51.1": "Dadāhi dāni me bhariyaṁ, ",
  "ja542:51.2": "nāriṁ sabbaṅgasobhiniṁ; ",
  "ja542:51.3": "Suvaṇṇena paṭicchannaṁ, ",
  "ja542:51.4": "dāsīgaṇapurakkhataṁ”. ",
  "ja542:52.1": "“Svāgataṁ teva vedeha, ",
  "ja542:52.2": "atho te adurāgataṁ; ",
  "ja542:52.3": "Nakkhattaṁyeva paripuccha, ",
  "ja542:52.4": "ahaṁ kaññaṁ dadāmi te; ",
  "ja542:52.5": "Suvaṇṇena paṭicchannaṁ, ",
  "ja542:52.6": "dāsīgaṇapurakkhataṁ”. ",
  "ja542:53.1": "Tato ca rājā vedeho, ",
  "ja542:53.2": "nakkhattaṁ paripucchatha; ",
  "ja542:53.3": "Nakkhattaṁ paripucchitvā, ",
  "ja542:53.4": "brahmadattassa pāhiṇi. ",
  "ja542:54.1": "“Dadāhi dāni me bhariyaṁ, ",
  "ja542:54.2": "nāriṁ sabbaṅgasobhiniṁ; ",
  "ja542:54.3": "Suvaṇṇena paṭicchannaṁ, ",
  "ja542:54.4": "dāsīgaṇapurakkhataṁ”. ",
  "ja542:55.1": "“Dadāmi dāni te bhariyaṁ, ",
  "ja542:55.2": "nāriṁ sabbaṅgasobhiniṁ; ",
  "ja542:55.3": "Suvaṇṇena paṭicchannaṁ, ",
  "ja542:55.4": "dāsīgaṇapurakkhataṁ”. ",
  "ja542:56.1": "“Hatthī assā rathā pattī, ",
  "ja542:56.2": "senā tiṭṭhanti vammitā; ",
  "ja542:56.3": "Ukkā padittā jhāyanti, ",
  "ja542:56.4": "kiṁ nu maññanti paṇḍitā. ",
  "ja542:57.1": "Hatthī assā rathā pattī, ",
  "ja542:57.2": "senā tiṭṭhanti vammitā; ",
  "ja542:57.3": "Ukkā padittā jhāyanti, ",
  "ja542:57.4": "kiṁ nu kāhanti paṇḍita”. ",
  "ja542:58.1": "“Rakkhati taṁ mahārāja, ",
  "ja542:58.2": "cūḷaneyyo mahabbalo; ",
  "ja542:58.3": "Paduṭṭho brahmadattena, ",
  "ja542:58.4": "pāto taṁ ghātayissati”. ",
  "ja542:59.1": "“Ubbedhati me hadayaṁ, ",
  "ja542:59.2": "mukhañca parisussati; ",
  "ja542:59.3": "Nibbutiṁ nādhigacchāmi, ",
  "ja542:59.4": "aggidaḍḍhova ātape. ",
  "ja542:60.1": "Kammārānaṁ yathā ukkā, ",
  "ja542:60.2": "anto jhāyati no bahi; ",
  "ja542:60.3": "Evampi hadayaṁ mayhaṁ, ",
  "ja542:60.4": "anto jhāyati no bahi”. ",
  "ja542:61.1": "“Pamatto mantanātīto, ",
  "ja542:61.2": "bhinnamantosi khattiya; ",
  "ja542:61.3": "Idāni kho taṁ tāyantu, ",
  "ja542:61.4": "paṇḍitā mantino janā. ",
  "ja542:62.1": "Akatvāmaccassa vacanaṁ, ",
  "ja542:62.2": "atthakāmahitesino; ",
  "ja542:62.3": "Attapītirato rājā, ",
  "ja542:62.4": "migo kūṭeva ohito. ",
  "ja542:63.1": "Yathāpi maccho baḷisaṁ, ",
  "ja542:63.2": "vaṅkaṁ maṁsena chāditaṁ; ",
  "ja542:63.3": "Āmagiddho na jānāti, ",
  "ja542:63.4": "maccho maraṇamattano. ",
  "ja542:64.1": "Evameva tuvaṁ rāja, ",
  "ja542:64.2": "cūḷaneyyassa dhītaraṁ; ",
  "ja542:64.3": "Kāmagiddho na jānāsi, ",
  "ja542:64.4": "macchova maraṇamattano. ",
  "ja542:65.1": "Sace gacchasi pañcālaṁ, ",
  "ja542:65.2": "khippamattaṁ jahissasi; ",
  "ja542:65.3": "Migaṁ panthānubandhaṁva, ",
  "ja542:65.4": "mahantaṁ bhayamessati. ",
  "ja542:66.1": "Anariyarūpo puriso janinda, ",
  "ja542:66.2": "Ahīva ucchaṅgagato ḍaseyya; ",
  "ja542:66.3": "Na tena mittiṁ kayirātha dhīro, ",
  "ja542:66.4": "Dukkho have kāpurisena saṅgamo. ",
  "ja542:67.1": "Yadeva jaññā purisaṁ janinda, ",
  "ja542:67.2": "Sīlavāyaṁ bahussuto; ",
  "ja542:67.3": "Teneva mittiṁ kayirātha dhīro, ",
  "ja542:67.4": "Sukho have sappurisena saṅgamo. ",
  "ja542:68.1": "Bālo tuvaṁ eḷamūgosi rāja, ",
  "ja542:68.2": "Yo uttamatthāni mayī lapittho; ",
  "ja542:68.3": "Kimevahaṁ naṅgalakoṭivaḍḍho, ",
  "ja542:68.4": "Atthāni jānāmi yathāpi aññe. ",
  "ja542:69.1": "Imaṁ gale gahetvāna, ",
  "ja542:69.2": "nāsetha vijitā mama; ",
  "ja542:69.3": "Yo me ratanalābhassa, ",
  "ja542:69.4": "antarāyāya bhāsati”. ",
  "ja542:70.1": "“Mahosadha atītena, ",
  "ja542:70.2": "Nānuvijjhanti paṇḍitā; ",
  "ja542:70.3": "Kiṁ maṁ assaṁva sambaddhaṁ, ",
  "ja542:70.4": "Patodeneva vijjhasi. ",
  "ja542:71.1": "Sace passasi mokkhaṁ vā, ",
  "ja542:71.2": "khemaṁ vā pana passasi; ",
  "ja542:71.3": "Teneva maṁ anusāsa, ",
  "ja542:71.4": "kiṁ atītena vijjhasi”. ",
  "ja542:72.1": "“Atītaṁ mānusaṁ kammaṁ, ",
  "ja542:72.2": "Dukkaraṁ durabhisambhavaṁ; ",
  "ja542:72.3": "Na taṁ sakkomi mocetuṁ, ",
  "ja542:72.4": "Tvaṁ pajānassu khattiya. ",
  "ja542:73.1": "Santi vehāyasā nāgā, ",
  "ja542:73.2": "iddhimanto yasassino; ",
  "ja542:73.3": "Tepi ādāya gaccheyyuṁ, ",
  "ja542:73.4": "yassa honti tathāvidhā. ",
  "ja542:74.1": "Santi vehāyasā assā, ",
  "ja542:74.2": "iddhimanto yasassino; ",
  "ja542:74.3": "Tepi ādāya gaccheyyuṁ, ",
  "ja542:74.4": "yassa honti tathāvidhā. ",
  "ja542:75.1": "Santi vehāyasā pakkhī, ",
  "ja542:75.2": "iddhimanto yasassino; ",
  "ja542:75.3": "Tepi ādāya gaccheyyuṁ, ",
  "ja542:75.4": "yassa honti tathāvidhā. ",
  "ja542:76.1": "Santi vehāyasā yakkhā, ",
  "ja542:76.2": "iddhimanto yasassino; ",
  "ja542:76.3": "Tepi ādāya gaccheyyuṁ, ",
  "ja542:76.4": "yassa honti tathāvidhā. ",
  "ja542:77.1": "Atītaṁ mānusaṁ kammaṁ, ",
  "ja542:77.2": "Dukkaraṁ durabhisambhavaṁ; ",
  "ja542:77.3": "Na taṁ sakkomi mocetuṁ, ",
  "ja542:77.4": "Antalikkhena khattiya”. ",
  "ja542:78.1": "“Atīradassī puriso, ",
  "ja542:78.2": "mahante udakaṇṇave; ",
  "ja542:78.3": "Yattha so labhate gādhaṁ, ",
  "ja542:78.4": "tattha so vindate sukhaṁ. ",
  "ja542:79.1": "Evaṁ amhañca rañño ca, ",
  "ja542:79.2": "tvaṁ patiṭṭhā mahosadha; ",
  "ja542:79.3": "Tvaṁ nosi mantinaṁ seṭṭho, ",
  "ja542:79.4": "amhe dukkhā pamocaya”. ",
  "ja542:80.1": "“Atītaṁ mānusaṁ kammaṁ, ",
  "ja542:80.2": "Dukkaraṁ durabhisambhavaṁ; ",
  "ja542:80.3": "Na taṁ sakkomi mocetuṁ, ",
  "ja542:80.4": "Tvaṁ pajānassu senaka”. ",
  "ja542:81.1": "“Suṇohi metaṁ vacanaṁ, ",
  "ja542:81.2": "Passa senaṁ mahabbhayaṁ; ",
  "ja542:81.3": "Senakaṁ dāni pucchāmi, ",
  "ja542:81.4": "Kiṁ kiccaṁ idha maññasi”. ",
  "ja542:82.1": "“Aggiṁ vā dvārato dema, ",
  "ja542:82.2": "gaṇhāmase vikantanaṁ; ",
  "ja542:82.3": "Aññamaññaṁ vadhitvāna, ",
  "ja542:82.4": "khippaṁ hissāma jīvitaṁ; ",
  "ja542:82.5": "Mā no rājā brahmadatto, ",
  "ja542:82.6": "ciraṁ dukkhena mārayi”. ",
  "ja542:83.1": "“Suṇohi metaṁ vacanaṁ, ",
  "ja542:83.2": "passa senaṁ mahabbhayaṁ; ",
  "ja542:83.3": "Pukkusaṁ dāni pucchāmi, ",
  "ja542:83.4": "kiṁ kiccaṁ idha maññasi”. ",
  "ja542:84.1": "“Visaṁ khāditvā miyyāma, ",
  "ja542:84.2": "khippaṁ hissāma jīvitaṁ; ",
  "ja542:84.3": "Mā no rājā brahmadatto, ",
  "ja542:84.4": "ciraṁ dukkhena mārayi”. ",
  "ja542:85.1": "“Suṇohi metaṁ vacanaṁ, ",
  "ja542:85.2": "passa senaṁ mahabbhayaṁ; ",
  "ja542:85.3": "Kāmindaṁ dāni pucchāmi, ",
  "ja542:85.4": "kiṁ kiccaṁ idha maññasi”. ",
  "ja542:86.1": "“Rajjuyā bajjha miyyāma, ",
  "ja542:86.2": "papātā papatāmase; ",
  "ja542:86.3": "Mā no rājā brahmadatto, ",
  "ja542:86.4": "ciraṁ dukkhena mārayi”. ",
  "ja542:87.1": "“Suṇohi metaṁ vacanaṁ, ",
  "ja542:87.2": "passa senaṁ mahabbhayaṁ; ",
  "ja542:87.3": "Devindaṁ dāni pucchāmi, ",
  "ja542:87.4": "kiṁ kiccaṁ idha maññasi”. ",
  "ja542:88.1": "“Aggiṁ vā dvārato dema, ",
  "ja542:88.2": "gaṇhāmase vikantanaṁ; ",
  "ja542:88.3": "Aññamaññaṁ vadhitvāna, ",
  "ja542:88.4": "khippaṁ hissāma jīvitaṁ; ",
  "ja542:88.5": "Na no sakkoti mocetuṁ, ",
  "ja542:88.6": "sukheneva mahosadho”. ",
  "ja542:89.1": "“Yathā kadalino sāraṁ, ",
  "ja542:89.2": "anvesaṁ nādhigacchati; ",
  "ja542:89.3": "Evaṁ anvesamānā naṁ, ",
  "ja542:89.4": "pañhaṁ najjhagamāmase. ",
  "ja542:90.1": "Yathā simbalino sāraṁ, ",
  "ja542:90.2": "anvesaṁ nādhigacchati; ",
  "ja542:90.3": "Evaṁ anvesamānā naṁ, ",
  "ja542:90.4": "pañhaṁ najjhagamāmase. ",
  "ja542:91.1": "Adese vata no vuṭṭhaṁ, ",
  "ja542:91.2": "kuñjarānaṁvanodake; ",
  "ja542:91.3": "Sakāse dummanussānaṁ, ",
  "ja542:91.4": "bālānaṁ avijānataṁ. ",
  "ja542:92.1": "Ubbedhati me hadayaṁ, ",
  "ja542:92.2": "mukhañca parisussati; ",
  "ja542:92.3": "Nibbutiṁ nādhigacchāmi, ",
  "ja542:92.4": "aggidaḍḍhova ātape. ",
  "ja542:93.1": "Kammārānaṁ yathā ukkā, ",
  "ja542:93.2": "anto jhāyati no bahi; ",
  "ja542:93.3": "Evampi hadayaṁ mayhaṁ, ",
  "ja542:93.4": "anto jhāyati no bahi”. ",
  "ja542:94.1": "Tato so paṇḍito dhīro, ",
  "ja542:94.2": "atthadassī mahosadho; ",
  "ja542:94.3": "Vedehaṁ dukkhitaṁ disvā, ",
  "ja542:94.4": "idaṁ vacanamabravi. ",
  "ja542:95.1": "“Mā tvaṁ bhāyi mahārāja, ",
  "ja542:95.2": "mā tvaṁ bhāyi rathesabha; ",
  "ja542:95.3": "Ahaṁ taṁ mocayissāmi, ",
  "ja542:95.4": "rāhuggahaṁva candimaṁ. ",
  "ja542:96.1": "Mā tvaṁ bhāyi mahārāja, ",
  "ja542:96.2": "mā tvaṁ bhāyi rathesabha; ",
  "ja542:96.3": "Ahaṁ taṁ mocayissāmi, ",
  "ja542:96.4": "rāhuggahaṁva sūriyaṁ. ",
  "ja542:97.1": "Mā tvaṁ bhāyi mahārāja, ",
  "ja542:97.2": "mā tvaṁ bhāyi rathesabha; ",
  "ja542:97.3": "Ahaṁ taṁ mocayissāmi, ",
  "ja542:97.4": "paṅke sannaṁva kuñjaraṁ. ",
  "ja542:98.1": "Mā tvaṁ bhāyi mahārāja, ",
  "ja542:98.2": "mā tvaṁ bhāyi rathesabha; ",
  "ja542:98.3": "Ahaṁ taṁ mocayissāmi, ",
  "ja542:98.4": "peḷābaddhaṁva pannagaṁ. ",
  "ja542:99.1": "Mā tvaṁ bhāyi mahārāja, ",
  "ja542:99.2": "mā tvaṁ bhāyi rathesabha; ",
  "ja542:99.3": "Ahaṁ taṁ mocayissāmi, ",
  "ja542:99.4": "pakkhiṁ baddhaṁva pañjare. ",
  "ja542:100.1": "Mā tvaṁ bhāyi mahārāja, ",
  "ja542:100.2": "mā tvaṁ bhāyi rathesabha; ",
  "ja542:100.3": "Ahaṁ taṁ mocayissāmi, ",
  "ja542:100.4": "macche jālagateriva. ",
  "ja542:101.1": "Mā tvaṁ bhāyi mahārāja, ",
  "ja542:101.2": "mā tvaṁ bhāyi rathesabha; ",
  "ja542:101.3": "Ahaṁ taṁ mocayissāmi, ",
  "ja542:101.4": "sayoggabalavāhanaṁ. ",
  "ja542:102.1": "Mā tvaṁ bhāyi mahārāja, ",
  "ja542:102.2": "mā tvaṁ bhāyi rathesabha; ",
  "ja542:102.3": "Pañcālaṁ vāhayissāmi, ",
  "ja542:102.4": "kākasenaṁva leḍḍunā. ",
  "ja542:103.1": "Adu paññā kimatthiyā, ",
  "ja542:103.2": "Amacco vāpi tādiso; ",
  "ja542:103.3": "Yo taṁ sambādhapakkhandaṁ, ",
  "ja542:103.4": "Dukkhā na parimocaye”. ",
  "ja542:104.1": "“Etha māṇavā uṭṭhetha, ",
  "ja542:104.2": "mukhaṁ sodhetha sandhino; ",
  "ja542:104.3": "Vedeho sahamaccehi, ",
  "ja542:104.4": "umaṅgena gamissati”. ",
  "ja542:105.1": "Tassa taṁ vacanaṁ sutvā, ",
  "ja542:105.2": "paṇḍitassānucārino; ",
  "ja542:105.3": "Umaṅgadvāraṁ vivariṁsu, ",
  "ja542:105.4": "yantayutte ca aggaḷe. ",
  "ja542:106.1": "Purato senako yāti, ",
  "ja542:106.2": "pacchato ca mahosadho; ",
  "ja542:106.3": "Majjhe ca rājā vedeho, ",
  "ja542:106.4": "amaccaparivārito. ",
  "ja542:107.1": "Umaṅgā nikkhamitvāna, ",
  "ja542:107.2": "vedeho nāvamāruhi; ",
  "ja542:107.3": "Abhirūḷhañca taṁ ñatvā, ",
  "ja542:107.4": "anusāsi mahosadho. ",
  "ja542:108.1": "“Ayaṁ te sasuro deva, ",
  "ja542:108.2": "ayaṁ sassu janādhipa; ",
  "ja542:108.3": "Yathā mātu paṭipatti, ",
  "ja542:108.4": "evaṁ te hotu sassuyā. ",
  "ja542:109.1": "Yathāpi niyako bhātā, ",
  "ja542:109.2": "Saudariyo ekamātuko; ",
  "ja542:109.3": "Evaṁ pañcālacando te, ",
  "ja542:109.4": "Dayitabbo rathesabha. ",
  "ja542:110.1": "Ayaṁ pañcālacandī te, ",
  "ja542:110.2": "rājaputtī abhicchitā; ",
  "ja542:110.3": "Kāmaṁ karohi te tāya, ",
  "ja542:110.4": "bhariyā te rathesabha”. ",
  "ja542:111.1": "“Āruyha nāvaṁ taramāno, ",
  "ja542:111.2": "kiṁ nu tīramhi tiṭṭhasi; ",
  "ja542:111.3": "Kicchā muttāmha dukkhato, ",
  "ja542:111.4": "yāma dāni mahosadha”. ",
  "ja542:112.1": "“Nesa dhammo mahārāja, ",
  "ja542:112.2": "yohaṁ senāya nāyako; ",
  "ja542:112.3": "Senaṅgaṁ parihāpetvā, ",
  "ja542:112.4": "attānaṁ parimocaye. ",
  "ja542:113.1": "Nivesanamhi te deva, ",
  "ja542:113.2": "senaṅgaṁ parihāpitaṁ; ",
  "ja542:113.3": "Taṁ dinnaṁ brahmadattena, ",
  "ja542:113.4": "ānayissaṁ rathesabha”. ",
  "ja542:114.1": "“Appaseno mahāsenaṁ, ",
  "ja542:114.2": "kathaṁ viggayha ṭhassasi; ",
  "ja542:114.3": "Dubbalo balavantena, ",
  "ja542:114.4": "vihaññissasi paṇḍita”. ",
  "ja542:115.1": "“Appasenopi ce mantī, ",
  "ja542:115.2": "mahāsenaṁ amantinaṁ; ",
  "ja542:115.3": "Jināti rājā rājāno, ",
  "ja542:115.4": "ādiccovudayaṁ tamaṁ”. ",
  "ja542:116.1": "“Susukhaṁ vata saṁvāso, ",
  "ja542:116.2": "paṇḍitehīti senaka; ",
  "ja542:116.3": "Pakkhīva pañjare baddhe, ",
  "ja542:116.4": "macche jālagateriva; ",
  "ja542:116.5": "Amittahatthattagate, ",
  "ja542:116.6": "mocayī no mahosadho”. ",
  "ja542:117.1": "“Evametaṁ mahārāja, ",
  "ja542:117.2": "paṇḍitā hi sukhāvahā; ",
  "ja542:117.3": "Pakkhīva pañjare baddhe, ",
  "ja542:117.4": "macche jālagateriva; ",
  "ja542:117.5": "Amittahatthattagate, ",
  "ja542:117.6": "mocayī no mahosadho”. ",
  "ja542:118.1": "Rakkhitvā kasiṇaṁ rattiṁ, ",
  "ja542:118.2": "cūḷaneyyo mahabbalo; ",
  "ja542:118.3": "Udentaṁ aruṇuggasmiṁ, ",
  "ja542:118.4": "upakāriṁ upāgami. ",
  "ja542:119.1": "Āruyha pavaraṁ nāgaṁ, ",
  "ja542:119.2": "balavantaṁ saṭṭhihāyanaṁ; ",
  "ja542:119.3": "Rājā avoca pañcālo, ",
  "ja542:119.4": "cūḷaneyyo mahabbalo. ",
  "ja542:120.1": "Sannaddho maṇivammena, ",
  "ja542:120.2": "saramādāya pāṇinā; ",
  "ja542:120.3": "Pesiye ajjhabhāsittha, ",
  "ja542:120.4": "puthugumbe samāgate. ",
  "ja542:121.1": "Hatthārohe anīkaṭṭhe, ",
  "ja542:121.2": "rathike pattikārake; ",
  "ja542:121.3": "Upāsanamhi katahatthe, ",
  "ja542:121.4": "vālavedhe samāgate. ",
  "ja542:122.1": "“Pesetha kuñjare dantī, ",
  "ja542:122.2": "balavante saṭṭhihāyane; ",
  "ja542:122.3": "Maddantu kuñjarā nagaraṁ, ",
  "ja542:122.4": "vedehena sumāpitaṁ. ",
  "ja542:123.1": "Vacchadantamukhā setā, ",
  "ja542:123.2": "tikkhaggā aṭṭhivedhino; ",
  "ja542:123.3": "Paṇunnā dhanuvegena, ",
  "ja542:123.4": "sampatantutarītarā. ",
  "ja542:124.1": "Māṇavā vammino sūrā, ",
  "ja542:124.2": "citradaṇḍayutāvudhā; ",
  "ja542:124.3": "Pakkhandino mahānāgā, ",
  "ja542:124.4": "hatthīnaṁ hontu sammukhā. ",
  "ja542:125.1": "Sattiyo teladhotāyo, ",
  "ja542:125.2": "accimantā pabhassarā; ",
  "ja542:125.3": "Vijjotamānā tiṭṭhantu, ",
  "ja542:125.4": "sataraṁsīva tārakā. ",
  "ja542:126.1": "Āvudhabalavantānaṁ, ",
  "ja542:126.2": "guṇikāyūradhārinaṁ; ",
  "ja542:126.3": "Etādisānaṁ yodhānaṁ, ",
  "ja542:126.4": "saṅgāme apalāyinaṁ; ",
  "ja542:126.5": "Vedeho kuto muccissati, ",
  "ja542:126.6": "sace pakkhīva kāhiti. ",
  "ja542:127.1": "Tiṁsa me purisanāvutyo, ",
  "ja542:127.2": "sabbevekekaniccitā; ",
  "ja542:127.3": "Yesaṁ samaṁ na passāmi, ",
  "ja542:127.4": "kevalaṁ mahimaṁ caraṁ. ",
  "ja542:128.1": "Nāgā ca kappitā dantī, ",
  "ja542:128.2": "balavanto saṭṭhihāyanā; ",
  "ja542:128.3": "Yesaṁ khandhesu sobhanti, ",
  "ja542:128.4": "kumārā cārudassanā. ",
  "ja542:129.1": "Pītālaṅkārā pītavasanā, ",
  "ja542:129.2": "Pītuttaranivāsanā; ",
  "ja542:129.3": "Nāgakhandhesu sobhanti, ",
  "ja542:129.4": "Devaputtāva nandane. ",
  "ja542:130.1": "Pāṭhīnavaṇṇā nettiṁsā, ",
  "ja542:130.2": "teladhotā pabhassarā; ",
  "ja542:130.3": "Niṭṭhitā naradhīrehi, ",
  "ja542:130.4": "samadhārā sunissitā. ",
  "ja542:131.1": "Vellālino vītamalā, ",
  "ja542:131.2": "sikkāyasamayā daḷā; ",
  "ja542:131.3": "Gahitā balavantehi, ",
  "ja542:131.4": "suppahārappahāribhi. ",
  "ja542:132.1": "Suvaṇṇatharusampannā, ",
  "ja542:132.2": "lohitakacchupadhāritā; ",
  "ja542:132.3": "Vivattamānā sobhanti, ",
  "ja542:132.4": "vijjuvabbhaghanantare. ",
  "ja542:133.1": "Paṭākā vammino sūrā, ",
  "ja542:133.2": "asicammassa kovidā; ",
  "ja542:133.3": "Dhanuggahā sikkhitarā, ",
  "ja542:133.4": "nāgakhandhe nipātino. ",
  "ja542:134.1": "Etādisehi parikkhitto, ",
  "ja542:134.2": "natthi mokkho ito tava; ",
  "ja542:134.3": "Pabhāvaṁ te na passāmi, ",
  "ja542:134.4": "yena tvaṁ mithilaṁ vaje”. ",
  "ja542:135.1": "“Kiṁ nu santaramānova, ",
  "ja542:135.2": "nāgaṁ pesesi kuñjaraṁ; ",
  "ja542:135.3": "Pahaṭṭharūpo āpatasi, ",
  "ja542:135.4": "siddhatthosmīti maññasi. ",
  "ja542:136.1": "Oharetaṁ dhanuṁ cāpaṁ, ",
  "ja542:136.2": "khurappaṁ paṭisaṁhara; ",
  "ja542:136.3": "Oharetaṁ subhaṁ vammaṁ, ",
  "ja542:136.4": "veḷuriyamaṇisanthataṁ”. ",
  "ja542:137.1": "“Pasannamukhavaṇṇosi, ",
  "ja542:137.2": "mitapubbañca bhāsasi; ",
  "ja542:137.3": "Hoti kho maraṇakāle, ",
  "ja542:137.4": "edisī vaṇṇasampadā”. ",
  "ja542:138.1": "“Moghaṁ te gajjitaṁ rāja, ",
  "ja542:138.2": "bhinnamantosi khattiya; ",
  "ja542:138.3": "Duggaṇhosi tayā rājā, ",
  "ja542:138.4": "khaḷuṅkeneva sindhavo. ",
  "ja542:139.1": "Tiṇṇo hiyyo rājā gaṅgaṁ, ",
  "ja542:139.2": "sāmacco saparijjano; ",
  "ja542:139.3": "Haṁsarājaṁ yathā dhaṅko, ",
  "ja542:139.4": "anujjavaṁ patissasi. ",
  "ja542:140.1": "Siṅgālā rattibhāgena, ",
  "ja542:140.2": "phullaṁ disvāna kiṁsukaṁ; ",
  "ja542:140.3": "Maṁsapesīti maññantā, ",
  "ja542:140.4": "paribyūḷhā migādhamā. ",
  "ja542:141.1": "Vītivattāsu rattīsu, ",
  "ja542:141.2": "uggatasmiṁ divākare; ",
  "ja542:141.3": "Kiṁsukaṁ phullitaṁ disvā, ",
  "ja542:141.4": "āsacchinnā migādhamā. ",
  "ja542:142.1": "Evameva tuvaṁ rāja, ",
  "ja542:142.2": "vedehaṁ parivāriya; ",
  "ja542:142.3": "Āsacchinno gamissasi, ",
  "ja542:142.4": "siṅgālā kiṁsukaṁ yathā”. ",
  "ja542:143.1": "“Imassa hatthe pāde ca, ",
  "ja542:143.2": "kaṇṇanāsañca chindatha; ",
  "ja542:143.3": "Yo me amittaṁ hatthagataṁ, ",
  "ja542:143.4": "vedehaṁ parimocayi. ",
  "ja542:144.1": "Imaṁ maṁsaṁva pātabyaṁ, ",
  "ja542:144.2": "sūle katvā pacantu naṁ; ",
  "ja542:144.3": "Yo me amittaṁ hatthagataṁ, ",
  "ja542:144.4": "vedehaṁ parimocayi. ",
  "ja542:145.1": "Yathāpi āsabhaṁ cammaṁ, ",
  "ja542:145.2": "pathabyā vitaniyyati; ",
  "ja542:145.3": "Sīhassa atho byagghassa, ",
  "ja542:145.4": "hoti saṅkusamāhataṁ. ",
  "ja542:146.1": "Evaṁ taṁ vitanitvāna, ",
  "ja542:146.2": "vedhayissāmi sattiyā; ",
  "ja542:146.3": "Yo me amittaṁ hatthagataṁ, ",
  "ja542:146.4": "vedehaṁ parimocayi”. ",
  "ja542:147.1": "“Sace me hatthe pāde ca, ",
  "ja542:147.2": "kaṇṇanāsañca checchasi; ",
  "ja542:147.3": "Evaṁ pañcālacandassa, ",
  "ja542:147.4": "vedeho chedayissati. ",
  "ja542:148.1": "Sace me hatthe pāde ca, ",
  "ja542:148.2": "kaṇṇanāsañca checchasi; ",
  "ja542:148.3": "Evaṁ pañcālacandiyā, ",
  "ja542:148.4": "vedeho chedayissati. ",
  "ja542:149.1": "Sace me hatthe pāde ca, ",
  "ja542:149.2": "kaṇṇanāsañca checchasi; ",
  "ja542:149.3": "Evaṁ nandāya deviyā, ",
  "ja542:149.4": "vedeho chedayissati. ",
  "ja542:150.1": "Sace me hatthe pāde ca, ",
  "ja542:150.2": "kaṇṇanāsañca checchasi; ",
  "ja542:150.3": "Evaṁ te puttadārassa, ",
  "ja542:150.4": "vedeho chedayissati. ",
  "ja542:151.1": "Sace maṁsaṁva pātabyaṁ, ",
  "ja542:151.2": "sūle katvā pacissasi; ",
  "ja542:151.3": "Evaṁ pañcālacandassa, ",
  "ja542:151.4": "vedeho pācayissati. ",
  "ja542:152.1": "Sace maṁsaṁva pātabyaṁ, ",
  "ja542:152.2": "sūle katvā pacissasi; ",
  "ja542:152.3": "Evaṁ pañcālacandiyā, ",
  "ja542:152.4": "vedeho pācayissati. ",
  "ja542:153.1": "Sace maṁsaṁva pātabyaṁ, ",
  "ja542:153.2": "sūle katvā pacissasi; ",
  "ja542:153.3": "Evaṁ nandāya deviyā, ",
  "ja542:153.4": "vedeho pācayissati. ",
  "ja542:154.1": "Sace maṁsaṁva pātabyaṁ, ",
  "ja542:154.2": "sūle katvā pacissasi; ",
  "ja542:154.3": "Evaṁ te puttadārassa, ",
  "ja542:154.4": "vedeho pācayissati. ",
  "ja542:155.1": "Sace maṁ vitanitvāna, ",
  "ja542:155.2": "vedhayissasi sattiyā; ",
  "ja542:155.3": "Evaṁ pañcālacandassa, ",
  "ja542:155.4": "vedeho vedhayissati. ",
  "ja542:156.1": "Sace maṁ vitanitvāna, ",
  "ja542:156.2": "vedhayissasi sattiyā; ",
  "ja542:156.3": "Evaṁ pañcālacandiyā, ",
  "ja542:156.4": "vedeho vedhayissati. ",
  "ja542:157.1": "Sace maṁ vitanitvāna, ",
  "ja542:157.2": "vedhayissasi sattiyā; ",
  "ja542:157.3": "Evaṁ nandāya deviyā, ",
  "ja542:157.4": "vedeho vedhayissati. ",
  "ja542:158.1": "Sace maṁ vitanitvāna, ",
  "ja542:158.2": "vedhayissasi sattiyā; ",
  "ja542:158.3": "Evaṁ te puttadārassa, ",
  "ja542:158.4": "vedeho vedhayissati; ",
  "ja542:158.5": "Evaṁ no mantitaṁ raho, ",
  "ja542:158.6": "vedehena mayā saha. ",
  "ja542:159.1": "Yathāpi palasataṁ cammaṁ, ",
  "ja542:159.2": "kontimantāsuniṭṭhitaṁ; ",
  "ja542:159.3": "Upeti tanutāṇāya, ",
  "ja542:159.4": "sarānaṁ paṭihantave. ",
  "ja542:160.1": "Sukhāvaho dukkhanudo, ",
  "ja542:160.2": "vedehassa yasassino; ",
  "ja542:160.3": "Matiṁ te paṭihaññāmi, ",
  "ja542:160.4": "usuṁ palasatena vā. ",
  "ja542:161.1": "Iṅgha passa mahārāja, ",
  "ja542:161.2": "suññaṁ antepuraṁ tava; ",
  "ja542:161.3": "Orodhā ca kumārā ca, ",
  "ja542:161.4": "tava mātā ca khattiya; ",
  "ja542:161.5": "Umaṅgā nīharitvāna, ",
  "ja542:161.6": "vedehassupanāmitā”. ",
  "ja542:162.1": "“Iṅgha antepuraṁ mayhaṁ, ",
  "ja542:162.2": "gantvāna vicinātha naṁ; ",
  "ja542:162.3": "Yathā imassa vacanaṁ, ",
  "ja542:162.4": "saccaṁ vā yadi vā musā”. ",
  "ja542:163.1": "“Evametaṁ mahārāja, ",
  "ja542:163.2": "yathā āha mahosadho; ",
  "ja542:163.3": "Suññaṁ antepuraṁ sabbaṁ, ",
  "ja542:163.4": "kākapaṭṭanakaṁ yathā”. ",
  "ja542:164.1": "“Ito gatā mahārāja, ",
  "ja542:164.2": "nārī sabbaṅgasobhanā; ",
  "ja542:164.3": "Kosambaphalakasussoṇī, ",
  "ja542:164.4": "haṁsagaggarabhāṇinī. ",
  "ja542:165.1": "Ito nītā mahārāja, ",
  "ja542:165.2": "nārī sabbaṅgasobhanā; ",
  "ja542:165.3": "Koseyyavasanā sāmā, ",
  "ja542:165.4": "jātarūpasumekhalā. ",
  "ja542:166.1": "Surattapādā kalyāṇī, ",
  "ja542:166.2": "suvaṇṇamaṇimekhalā; ",
  "ja542:166.3": "Pārevatakkhī sutanū, ",
  "ja542:166.4": "bimboṭṭhā tanumajjhimā. ",
  "ja542:167.1": "Sujātā bhujalaṭṭhīva, ",
  "ja542:167.2": "vedīva tanumajjhimā; ",
  "ja542:167.3": "Dīghassā kesā asitā, ",
  "ja542:167.4": "īsakaggapavellitā. ",
  "ja542:168.1": "Sujātā migachāpāva, ",
  "ja542:168.2": "hemantaggisikhāriva; ",
  "ja542:168.3": "Nadīva giriduggesu, ",
  "ja542:168.4": "sañchannā khuddaveḷubhi. ",
  "ja542:169.1": "Nāganāsūru kalyāṇī, ",
  "ja542:169.2": "paramā timbarutthanī; ",
  "ja542:169.3": "Nātidīghā nātirassā, ",
  "ja542:169.4": "nālomā nātilomasā”. ",
  "ja542:170.1": "“Nandāya nūna maraṇena, ",
  "ja542:170.2": "nandasi sirivāhana; ",
  "ja542:170.3": "Ahañca nūna nandā ca, ",
  "ja542:170.4": "gacchāma yamasādhanaṁ”. ",
  "ja542:171.1": "“Dibbaṁ adhīyase māyaṁ, ",
  "ja542:171.2": "akāsi cakkhumohanaṁ; ",
  "ja542:171.3": "Yo me amittaṁ hatthagataṁ, ",
  "ja542:171.4": "vedehaṁ parimocayi”. ",
  "ja542:172.1": "“Adhīyanti mahārāja, ",
  "ja542:172.2": "dibbamāyidha paṇḍitā; ",
  "ja542:172.3": "Te mocayanti attānaṁ, ",
  "ja542:172.4": "paṇḍitā mantino janā. ",
  "ja542:173.1": "Santi māṇavaputtā me, ",
  "ja542:173.2": "kusalā sandhichedakā; ",
  "ja542:173.3": "Yesaṁ katena maggena, ",
  "ja542:173.4": "vedeho mithilaṁ gato”. ",
  "ja542:174.1": "“Iṅgha passa mahārāja, ",
  "ja542:174.2": "Umaṅgaṁ sādhu māpitaṁ; ",
  "ja542:174.3": "Hatthīnaṁ atha assānaṁ, ",
  "ja542:174.4": "Rathānaṁ atha pattinaṁ; ",
  "ja542:174.5": "Ālokabhūtaṁ tiṭṭhantaṁ, ",
  "ja542:174.6": "Umaṅgaṁ sādhu māpitaṁ”. ",
  "ja542:175.1": "“Lābhā vata videhānaṁ, ",
  "ja542:175.2": "yassimedisā paṇḍitā; ",
  "ja542:175.3": "Ghare vasanti vijite, ",
  "ja542:175.4": "yathā tvaṁsi mahosadha”. ",
  "ja542:176.1": "“Vuttiñca parihārañca, ",
  "ja542:176.2": "diguṇaṁ bhattavetanaṁ; ",
  "ja542:176.3": "Dadāmi vipule bhoge, ",
  "ja542:176.4": "bhuñja kāme ramassu ca; ",
  "ja542:176.5": "Mā videhaṁ paccagamā, ",
  "ja542:176.6": "kiṁ videho karissati”. ",
  "ja542:177.1": "“Yo cajetha mahārāja, ",
  "ja542:177.2": "bhattāraṁ dhanakāraṇā; ",
  "ja542:177.3": "Ubhinnaṁ hoti gārayho, ",
  "ja542:177.4": "attano ca parassa ca; ",
  "ja542:177.5": "Yāva jīveyya vedeho, ",
  "ja542:177.6": "nāññassa puriso siyā. ",
  "ja542:178.1": "Yo cajetha mahārāja, ",
  "ja542:178.2": "bhattāraṁ dhanakāraṇā; ",
  "ja542:178.3": "Ubhinnaṁ hoti gārayho, ",
  "ja542:178.4": "attano ca parassa ca; ",
  "ja542:178.5": "Yāva tiṭṭheyya vedeho, ",
  "ja542:178.6": "nāññassa vijite vase”. ",
  "ja542:179.1": "“Dammi nikkhasahassaṁ te, ",
  "ja542:179.2": "gāmāsītiñca kāsisu; ",
  "ja542:179.3": "Dāsisatāni cattāri, ",
  "ja542:179.4": "dammi bhariyāsatañca te; ",
  "ja542:179.5": "Sabbaṁ senaṅgamādāya, ",
  "ja542:179.6": "sotthiṁ gaccha mahosadha. ",
  "ja542:180.1": "Yāva dadantu hatthīnaṁ, ",
  "ja542:180.2": "assānaṁ diguṇaṁ vidhaṁ; ",
  "ja542:180.3": "Tappentu annapānena, ",
  "ja542:180.4": "rathike pattikārake. ",
  "ja542:181.1": "Hatthī asse rathe pattī, ",
  "ja542:181.2": "gacchevādāya paṇḍita; ",
  "ja542:181.3": "Passatu taṁ mahārājā, ",
  "ja542:181.4": "vedeho mithilaṁ gataṁ”. ",
  "ja542:182.1": "“Hatthī assā rathā pattī, ",
  "ja542:182.2": "senā padissate mahā; ",
  "ja542:182.3": "Caturaṅginī bhīsarūpā, ",
  "ja542:182.4": "kiṁ nu maññasi paṇḍita”. ",
  "ja542:183.1": "“Ānando te mahārāja, ",
  "ja542:183.2": "uttamo paṭidissati; ",
  "ja542:183.3": "Sabbaṁ senaṅgamādāya, ",
  "ja542:183.4": "sotthiṁ patto mahosadho”. ",
  "ja542:184.1": "“Yathā petaṁ susānasmiṁ, ",
  "ja542:184.2": "chaḍḍetvā caturo janā; ",
  "ja542:184.3": "Evaṁ kapilaye tyamha, ",
  "ja542:184.4": "chaḍḍayitvā idhāgatā. ",
  "ja542:185.1": "Atha tvaṁ kena vaṇṇena, ",
  "ja542:185.2": "kena vā pana hetunā; ",
  "ja542:185.3": "Kena vā atthajātena, ",
  "ja542:185.4": "attānaṁ parimocayi”. ",
  "ja542:186.1": "“Atthaṁ atthena vedeha, ",
  "ja542:186.2": "mantaṁ mantena khattiya; ",
  "ja542:186.3": "Parivārayiṁ rājānaṁ, ",
  "ja542:186.4": "jambudīpaṁva sāgaro”. ",
  "ja542:187.1": "“Dinnaṁ nikkhasahassaṁ me, ",
  "ja542:187.2": "gāmāsīti ca kāsisu; ",
  "ja542:187.3": "Dāsīsatāni cattāri, ",
  "ja542:187.4": "dinnaṁ bhariyāsatañca me; ",
  "ja542:187.5": "Sabbaṁ senaṅgamādāya, ",
  "ja542:187.6": "sotthināmhi idhāgato”. ",
  "ja542:188.1": "“Susukhaṁ vata saṁvāso, ",
  "ja542:188.2": "paṇḍitehīti senaka; ",
  "ja542:188.3": "Pakkhīva pañjare baddhe, ",
  "ja542:188.4": "macche jālagateriva; ",
  "ja542:188.5": "Amittahatthattagate, ",
  "ja542:188.6": "mocayī no mahosadho”. ",
  "ja542:189.1": "“Evametaṁ mahārāja, ",
  "ja542:189.2": "paṇḍitā hi sukhāvahā; ",
  "ja542:189.3": "Pakkhīva pañjare baddhe, ",
  "ja542:189.4": "macche jālagateriva; ",
  "ja542:189.5": "Amittahatthattagate, ",
  "ja542:189.6": "mocayī no mahosadho”. ",
  "ja542:190.1": "“Āhaññantu sabbavīṇā, ",
  "ja542:190.2": "bheriyo dindimāni ca; ",
  "ja542:190.3": "Dhamentu māgadhā saṅkhā, ",
  "ja542:190.4": "vaggū nadantu dundubhī”. ",
  "ja542:191.1": "Orodhā ca kumārā ca, ",
  "ja542:191.2": "vesiyānā ca brāhmaṇā; ",
  "ja542:191.3": "Bahuṁ annañca pānañca, ",
  "ja542:191.4": "paṇḍitassābhihārayuṁ. ",
  "ja542:192.1": "Hatthārohā anīkaṭṭhā, ",
  "ja542:192.2": "rathikā pattikārakā; ",
  "ja542:192.3": "Bahuṁ annañca pānañca, ",
  "ja542:192.4": "paṇḍitassābhihārayuṁ. ",
  "ja542:193.1": "Samāgatā jānapadā, ",
  "ja542:193.2": "negamā ca samāgatā; ",
  "ja542:193.3": "Bahuṁ annañca pānañca, ",
  "ja542:193.4": "paṇḍitassābhihārayuṁ. ",
  "ja542:194.1": "Bahujano pasannosi, ",
  "ja542:194.2": "disvā paṇḍitamāgataṁ; ",
  "ja542:194.3": "Paṇḍitamhi anuppatte, ",
  "ja542:194.4": "celukkhepo avattathāti. ",
  "ja542:195.1": "Umaṅgajātakaṁ pañcamaṁ. "
}