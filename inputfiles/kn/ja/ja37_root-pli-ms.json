{
"ja37:1.1": "Jātaka",
"ja37:2.1": "Ekakanipāta",
"ja37:3.1": "Kulāvakavagga",
"ja37:4.1": "7. Tittirajātaka",
"ja37:5.1": "“Ye vuḍḍhamapacāyanti,",
"ja37:5.2": "narā dhammassa kovidā;",
"ja37:5.3": "Diṭṭheva dhamme pāsaṃsā,",
"ja37:5.4": "samparāye ca suggatī”ti.",
"ja37:6.1": "Tittirajātakaṃ sattamaṃ."
}