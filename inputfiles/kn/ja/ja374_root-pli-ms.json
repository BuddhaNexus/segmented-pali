{
  "ja374:0.1": "Jātaka ",
  "ja374:0.2": "Pañcakanipāta ",
  "ja374:0.3": "Aḍḍhavagga ",
  "ja374:0.4": "4. Cūḷadhanuggahajātaka ",
  "ja374:1.1": "“Sabbaṁ bhaṇḍaṁ samādāya, ",
  "ja374:1.2": "pāraṁ tiṇṇosi brāhmaṇa; ",
  "ja374:1.3": "Paccāgaccha lahuṁ khippaṁ, ",
  "ja374:1.4": "mampi tārehi dānito”. ",
  "ja374:2.1": "“Asanthutaṁ maṁ cirasanthutena, ",
  "ja374:2.2": "Nimīni bhotī addhuvaṁ dhuvena; ",
  "ja374:2.3": "Mayāpi bhotī nimineyya aññaṁ, ",
  "ja374:2.4": "Ito ahaṁ dūrataraṁ gamissaṁ”. ",
  "ja374:3.1": "“Kāyaṁ eḷagalāgumbe, ",
  "ja374:3.2": "karoti ahuhāsiyaṁ; ",
  "ja374:3.3": "Nayīdha naccagītaṁ vā, ",
  "ja374:3.4": "tāḷaṁ vā susamāhitaṁ; ",
  "ja374:3.5": "Anamhikāle susoṇi, ",
  "ja374:3.6": "kiṁ nu jagghasi sobhane”. ",
  "ja374:4.1": "“Siṅgāla bāla dummedha, ",
  "ja374:4.2": "appapaññosi jambuka; ",
  "ja374:4.3": "Jīno macchañca pesiñca, ",
  "ja374:4.4": "kapaṇo viya jhāyasi”. ",
  "ja374:5.1": "“Sudassaṁ vajjamaññesaṁ, ",
  "ja374:5.2": "attano pana duddasaṁ; ",
  "ja374:5.3": "Jīnā patiñca jārañca, ",
  "ja374:5.4": "maññe tvaññeva jhāyasi”. ",
  "ja374:6.1": "“Evametaṁ migarāja, ",
  "ja374:6.2": "yathā bhāsasi jambuka; ",
  "ja374:6.3": "Sā nūnāhaṁ ito gantvā, ",
  "ja374:6.4": "bhattu hessaṁ vasānugā”. ",
  "ja374:7.1": "“Yo hare mattikaṁ thālaṁ, ",
  "ja374:7.2": "kaṁsathālampi so hare; ",
  "ja374:7.3": "Katañceva tayā pāpaṁ, ",
  "ja374:7.4": "punapevaṁ karissasī”ti. ",
  "ja374:8.1": "Cūḷadhanuggahajātakaṁ catutthaṁ. "
}