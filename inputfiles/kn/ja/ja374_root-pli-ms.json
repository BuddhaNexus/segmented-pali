{
"ja374:1.1": "Jātaka",
"ja374:2.1": "Pañcakanipāta",
"ja374:3.1": "Aḍḍhavagga",
"ja374:4.1": "4. Cūḷadhanuggahajātaka",
"ja374:5.1": "“Sabbaṃ bhaṇḍaṃ samādāya,",
"ja374:5.2": "pāraṃ tiṇṇosi brāhmaṇa;",
"ja374:5.3": "Paccāgaccha lahuṃ khippaṃ,",
"ja374:5.4": "mampi tārehi dānito”.",
"ja374:6.1": "“Asanthutaṃ maṃ cirasanthutena,",
"ja374:6.2": "Nimīni bhotī addhuvaṃ dhuvena;",
"ja374:6.3": "Mayāpi bhotī nimineyya aññaṃ,",
"ja374:6.4": "Ito ahaṃ dūrataraṃ gamissaṃ”.",
"ja374:7.1": "“Kāyaṃ eḷagalāgumbe,",
"ja374:7.2": "karoti ahuhāsiyaṃ;",
"ja374:7.3": "Nayīdha naccagītaṃ vā,",
"ja374:7.4": "tāḷaṃ vā susamāhitaṃ;",
"ja374:7.5": "Anamhikāle susoṇi,",
"ja374:7.6": "kiṃ nu jagghasi sobhane”.",
"ja374:8.1": "“Siṅgāla bāla dummedha,",
"ja374:8.2": "appapaññosi jambuka;",
"ja374:8.3": "Jīno macchañca pesiñca,",
"ja374:8.4": "kapaṇo viya jhāyasi”.",
"ja374:9.1": "“Sudassaṃ vajjamaññesaṃ,",
"ja374:9.2": "attano pana duddasaṃ;",
"ja374:9.3": "Jīnā patiñca jārañca,",
"ja374:9.4": "maññe tvaññeva jhāyasi”.",
"ja374:10.1": "“Evametaṃ migarāja,",
"ja374:10.2": "yathā bhāsasi jambuka;",
"ja374:10.3": "Sā nūnāhaṃ ito gantvā,",
"ja374:10.4": "bhattu hessaṃ vasānugā”.",
"ja374:11.1": "“Yo hare mattikaṃ thālaṃ,",
"ja374:11.2": "kaṃsathālampi so hare;",
"ja374:11.3": "Katañceva tayā pāpaṃ,",
"ja374:11.4": "punapevaṃ karissasī”ti.",
"ja374:12.1": "Cūḷadhanuggahajātakaṃ catutthaṃ."
}