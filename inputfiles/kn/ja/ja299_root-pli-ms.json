{
"ja299:1.1": "Jātaka",
"ja299:2.1": "Tikanipāta",
"ja299:3.1": "Kumbhavagga",
"ja299:4.1": "9. Komāraputtajātaka",
"ja299:5.1": "“Pure tuvaṃ sīlavataṃ sakāse,",
"ja299:5.2": "Okkantikaṃ kīḷasi assamamhi;",
"ja299:5.3": "Karohare makkaṭiyāni makkaṭa,",
"ja299:5.4": "Na taṃ mayaṃ sīlavataṃ ramāma”.",
"ja299:6.1": "“Sutā hi mayhaṃ paramā visuddhi,",
"ja299:6.2": "Komāraputtassa bahussutassa;",
"ja299:6.3": "Mā dāni maṃ maññi tuvaṃ yathā pure,",
"ja299:6.4": "Jhānānuyutto viharāmi āvuso”.",
"ja299:7.1": "“Sacepi selasmi vapeyya bījaṃ,",
"ja299:7.2": "Devo ca vasse na hi taṃ virūḷhe;",
"ja299:7.3": "Sutā hi te sā paramā visuddhi,",
"ja299:7.4": "Ārā tuvaṃ makkaṭa jhānabhūmiyā”ti.",
"ja299:8.1": "Komāraputtajātakaṃ navamaṃ."
}