{
  "ja209:0.1": "Jātaka ",
  "ja209:0.2": "Dukanipāta ",
  "ja209:0.3": "Nataṁdaḷhavagga ",
  "ja209:0.4": "9. Kukkuṭajātaka ",
  "ja209:1.1": "“Diṭṭhā mayā vane rukkhā, ",
  "ja209:1.2": "assakaṇṇā vibhīṭakā; ",
  "ja209:1.3": "Na tāni evaṁ sakkanti, ",
  "ja209:1.4": "yathā tvaṁ rukkha sakkasi”. ",
  "ja209:2.1": "“Purāṇakukkuṭo ayaṁ, ",
  "ja209:2.2": "bhetvā pañjaramāgato; ",
  "ja209:2.3": "Kusalo vāḷapāsānaṁ, ",
  "ja209:2.4": "apakkamati bhāsatī”ti. ",
  "ja209:3.1": "Kukkuṭajātakaṁ navamaṁ. "
}