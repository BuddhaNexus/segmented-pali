{
"ja281:1.1": "Jātaka",
"ja281:2.1": "Tikanipāta",
"ja281:3.1": "Abbhantaravagga",
"ja281:4.1": "1. Abbhantarajātaka",
"ja281:5.1": "“Abbhantaro nāma dumo,",
"ja281:5.2": "yassa dibyamidaṃ phalaṃ;",
"ja281:5.3": "Bhutvā dohaḷinī nārī,",
"ja281:5.4": "cakkavattiṃ vijāyati.",
"ja281:6.1": "Tvampi bhadde mahesīsi,",
"ja281:6.2": "sā cāpi patino piyā;",
"ja281:6.3": "Āharissati te rājā,",
"ja281:6.4": "idaṃ abbhantaraṃ phalaṃ”.",
"ja281:7.1": "“Bhatturatthe parakkanto,",
"ja281:7.2": "yaṃ ṭhānamadhigacchati;",
"ja281:7.3": "Sūro attapariccāgī,",
"ja281:7.4": "labhamāno bhavāmahan”ti.",
"ja281:8.1": "Abbhantarajātakaṃ paṭhamaṃ."
}