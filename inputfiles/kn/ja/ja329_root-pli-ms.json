{
"ja329:1.1": "Jātaka",
"ja329:2.1": "Catukkanipāta",
"ja329:3.1": "Kuṭidūsakavagga",
"ja329:4.1": "9. Kāḷabāhujātaka",
"ja329:5.1": "“Yaṃ annapānassa pure labhāma,",
"ja329:5.2": "Taṃ dāni sākhamigameva gacchati;",
"ja329:5.3": "Gacchāma dāni vanameva rādha,",
"ja329:5.4": "Asakkatā casma dhanañjayāya”.",
"ja329:6.1": "“Lābho alābho yaso ayaso ca,",
"ja329:6.2": "Nindā pasaṃsā ca sukhañca dukkhaṃ;",
"ja329:6.3": "Ete aniccā manujesu dhammā,",
"ja329:6.4": "Mā soci kiṃ socasi poṭṭhapāda”.",
"ja329:7.1": "“Addhā tuvaṃ paṇḍitakosi rādha,",
"ja329:7.2": "Jānāsi atthāni anāgatāni;",
"ja329:7.3": "Kathaṃ nu sākhāmigaṃ dakkhisāma,",
"ja329:7.4": "Niddhāvitaṃ rājakulatova jammaṃ”.",
"ja329:8.1": "“Cāleti kaṇṇaṃ bhakuṭiṃ karoti,",
"ja329:8.2": "Muhuṃ muhuṃ bhāyayate kumāre;",
"ja329:8.3": "Sayameva taṃ kāhati kāḷabāhu,",
"ja329:8.4": "Yenārakā ṭhassati annapānā”ti.",
"ja329:9.1": "Kāḷabāhujātakaṃ navamaṃ."
}