{
"ja331:1.1": "Jātaka",
"ja331:2.1": "Catukkanipāta",
"ja331:3.1": "Kokilavagga",
"ja331:4.1": "1. Kokilajātaka",
"ja331:5.1": "“Yo ve kāle asampatte,",
"ja331:5.2": "ativelaṃ pabhāsati;",
"ja331:5.3": "Evaṃ so nihato seti,",
"ja331:5.4": "kokilāyiva atrajo.",
"ja331:6.1": "Na hi satthaṃ sunisitaṃ,",
"ja331:6.2": "visaṃ halāhalāmiva;",
"ja331:6.3": "Evaṃ nikaṭṭhe pāteti,",
"ja331:6.4": "vācā dubbhāsitā yathā.",
"ja331:7.1": "Tasmā kāle akāle vā,",
"ja331:7.2": "vācaṃ rakkheyya paṇḍito;",
"ja331:7.3": "Nātivelaṃ pabhāseyya,",
"ja331:7.4": "api attasamamhi vā.",
"ja331:8.1": "Yo ca kāle mitaṃ bhāse,",
"ja331:8.2": "matipubbo vicakkhaṇo;",
"ja331:8.3": "Sabbe amitte ādeti,",
"ja331:8.4": "supaṇṇo uragāmivā”ti.",
"ja331:9.1": "Kokilajātakaṃ paṭhamaṃ."
}