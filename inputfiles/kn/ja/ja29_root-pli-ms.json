{
"ja29:1.1": "Jātaka",
"ja29:2.1": "Ekakanipāta",
"ja29:3.1": "Kuruṅgavagga",
"ja29:4.1": "9. Kaṇhajātaka",
"ja29:5.1": "“Yato yato garu dhuraṃ,",
"ja29:5.2": "yato gambhīravattanī;",
"ja29:5.3": "Tadāssu kaṇhaṃ yuñjanti,",
"ja29:5.4": "svāssu taṃ vahate dhuran”ti.",
"ja29:6.1": "Kaṇhajātakaṃ navamaṃ."
}