{
"ja103:1.1": "Jātaka",
"ja103:2.1": "Ekakanipāta",
"ja103:3.1": "Parosatavagga",
"ja103:4.1": "3. Verijātaka",
"ja103:5.1": "“Yattha verī nivisati,",
"ja103:5.2": "na vase tattha paṇḍito;",
"ja103:5.3": "Ekarattaṃ dirattaṃ vā,",
"ja103:5.4": "dukkhaṃ vasati verisū”ti.",
"ja103:6.1": "Verijātakaṃ tatiyaṃ."
}