{
"ja223:1.1": "Jātaka",
"ja223:2.1": "Dukanipāta",
"ja223:3.1": "Kāsāvavagga",
"ja223:4.1": "3. Puṭabhattajātaka",
"ja223:5.1": "“Name namantassa bhaje bhajantaṃ,",
"ja223:5.2": "Kiccānukubbassa kareyya kiccaṃ;",
"ja223:5.3": "Nānatthakāmassa kareyya atthaṃ,",
"ja223:5.4": "Asambhajantampi na sambhajeyya.",
"ja223:6.1": "Caje cajantaṃ vanathaṃ na kayirā,",
"ja223:6.2": "Apetacittena na sambhajeyya;",
"ja223:6.3": "Dijo dumaṃ khīṇaphalanti ñatvā,",
"ja223:6.4": "Aññaṃ samekkheyya mahā hi loko”ti.",
"ja223:7.1": "Puṭabhattajātakaṃ tatiyaṃ."
}