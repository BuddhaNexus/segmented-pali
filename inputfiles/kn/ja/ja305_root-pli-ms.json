{
  "ja305:0.1": "Jātaka ",
  "ja305:0.2": "Catukkanipāta ",
  "ja305:0.3": "Kāliṅgavagga ",
  "ja305:0.4": "5. Sīlavīmaṁsanajātaka ",
  "ja305:1.1": "“Natthi loke raho nāma, ",
  "ja305:1.2": "pāpakammaṁ pakubbato; ",
  "ja305:1.3": "Passanti vanabhūtāni, ",
  "ja305:1.4": "taṁ bālo maññatī raho. ",
  "ja305:2.1": "Ahaṁ raho na passāmi, ",
  "ja305:2.2": "suññaṁ vāpi na vijjati; ",
  "ja305:2.3": "Yattha aññaṁ na passāmi, ",
  "ja305:2.4": "asuññaṁ hoti taṁ mayā”. ",
  "ja305:3.1": "“Dujjacco ca sujacco ca, ",
  "ja305:3.2": "nando ca sukhavaḍḍhito; ",
  "ja305:3.3": "Vejjo ca addhuvasīlo ca, ",
  "ja305:3.4": "te dhammaṁ jahu matthikā. ",
  "ja305:4.1": "Brāhmaṇo ca kathaṁ jahe, ",
  "ja305:4.2": "sabbadhammāna pāragū; ",
  "ja305:4.3": "Yo dhammamanupāleti, ",
  "ja305:4.4": "dhitimā saccanikkamo”ti. ",
  "ja305:5.1": "Sīlavīmaṁsanajātakaṁ pañcamaṁ. "
}