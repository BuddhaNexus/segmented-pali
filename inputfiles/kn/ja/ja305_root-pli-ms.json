{
"ja305:1.1": "Jātaka",
"ja305:2.1": "Catukkanipāta",
"ja305:3.1": "Kāliṅgavagga",
"ja305:4.1": "5. Sīlavīmaṃsanajātaka",
"ja305:5.1": "“Natthi loke raho nāma,",
"ja305:5.2": "pāpakammaṃ pakubbato;",
"ja305:5.3": "Passanti vanabhūtāni,",
"ja305:5.4": "taṃ bālo maññatī raho.",
"ja305:6.1": "Ahaṃ raho na passāmi,",
"ja305:6.2": "suññaṃ vāpi na vijjati;",
"ja305:6.3": "Yattha aññaṃ na passāmi,",
"ja305:6.4": "asuññaṃ hoti taṃ mayā”.",
"ja305:7.1": "“Dujjacco ca sujacco ca,",
"ja305:7.2": "nando ca sukhavaḍḍhito;",
"ja305:7.3": "Vejjo ca addhuvasīlo ca,",
"ja305:7.4": "te dhammaṃ jahu matthikā.",
"ja305:8.1": "Brāhmaṇo ca kathaṃ jahe,",
"ja305:8.2": "sabbadhammāna pāragū;",
"ja305:8.3": "Yo dhammamanupāleti,",
"ja305:8.4": "dhitimā saccanikkamo”ti.",
"ja305:9.1": "Sīlavīmaṃsanajātakaṃ pañcamaṃ."
}