{
"ja220:1.1": "Jātaka",
"ja220:2.1": "Dukanipāta",
"ja220:3.1": "Bīraṇathambhavagga",
"ja220:4.1": "10. Dhammadhajajātaka",
"ja220:5.1": "“Sukhaṃ jīvitarūposi,",
"ja220:5.2": "raṭṭhā vivanamāgato;",
"ja220:5.3": "So ekako rukkhamūle,",
"ja220:5.4": "kapaṇo viya jhāyasi”.",
"ja220:6.1": "“Sukhaṃ jīvitarūposmi,",
"ja220:6.2": "raṭṭhā vivanamāgato;",
"ja220:6.3": "So ekako rukkhamūle,",
"ja220:6.4": "kapaṇo viya jhāyāmi;",
"ja220:6.5": "Sataṃ dhammaṃ anussaran”ti.",
"ja220:7.1": "Dhammadhajajātakaṃ dasamaṃ.",
"ja220:7.2": "Bīraṇathambhavaggo sattamo.",
"ja220:8.1": "Tassuddānaṃ",
"ja220:9.1": "Atha bīraṇathambhavaro ca naṭo,",
"ja220:9.2": "Bharurājavaruttamapuṇṇanadī;",
"ja220:9.3": "Bahubhāṇi aggipavane mūsikā,",
"ja220:9.4": "Sahalambatthano kapaṇena dasāti."
}