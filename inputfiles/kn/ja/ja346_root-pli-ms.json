{
  "ja346:0.1": "Jātaka ",
  "ja346:0.2": "Catukkanipāta ",
  "ja346:0.3": "Cūḷakuṇālavagga ",
  "ja346:0.4": "6. Kesavajātaka ",
  "ja346:1.1": "“Manussindaṁ jahitvāna, ",
  "ja346:1.2": "sabbakāmasamiddhinaṁ; ",
  "ja346:1.3": "Kathaṁ nu bhagavā kesī, ",
  "ja346:1.4": "kappassa ramati assame”. ",
  "ja346:2.1": "“Sādūni ramaṇīyāni, ",
  "ja346:2.2": "santi vakkhā manoramā; ",
  "ja346:2.3": "Subhāsitāni kappassa, ",
  "ja346:2.4": "nārada ramayanti maṁ”. ",
  "ja346:3.1": "“Sālīnaṁ odanaṁ bhuñje, ",
  "ja346:3.2": "suciṁ maṁsūpasecanaṁ; ",
  "ja346:3.3": "Kathaṁ sāmākanīvāraṁ, ",
  "ja346:3.4": "aloṇaṁ chādayanti taṁ”. ",
  "ja346:4.1": "“Sāduṁ vā yadi vāsāduṁ, ",
  "ja346:4.2": "appaṁ vā yadi vā bahuṁ; ",
  "ja346:4.3": "Vissattho yattha bhuñjeyya, ",
  "ja346:4.4": "vissāsaparamā rasā”ti. ",
  "ja346:5.1": "Kesavajātakaṁ chaṭṭhaṁ. "
}