{
"ja203:1.1": "Jātaka",
"ja203:2.1": "Dukanipāta",
"ja203:3.1": "Nataṃdaḷhavagga",
"ja203:4.1": "3. Khaṇḍajātaka",
"ja203:5.1": "“Virūpakkhehi me mettaṃ,",
"ja203:5.2": "Mettaṃ erāpathehi me;",
"ja203:5.3": "Chabyāputtehi me mettaṃ,",
"ja203:5.4": "Mettaṃ kaṇhāgotamakehi ca.",
"ja203:6.1": "Apādakehi me mettaṃ,",
"ja203:6.2": "mettaṃ dvipādakehi me;",
"ja203:6.3": "Catuppadehi me mettaṃ,",
"ja203:6.4": "mettaṃ bahuppadehi me.",
"ja203:7.1": "Mā maṃ apādako hiṃsi,",
"ja203:7.2": "mā maṃ hiṃsi dvipādako;",
"ja203:7.3": "Mā maṃ catuppado hiṃsi,",
"ja203:7.4": "mā maṃ hiṃsi bahuppado.",
"ja203:8.1": "Sabbe sattā sabbe pāṇā,",
"ja203:8.2": "sabbe bhūtā ca kevalā;",
"ja203:8.3": "Sabbe bhadrāni passantu,",
"ja203:8.4": "mā kañci pāpamāgamā.",
"ja203:9.1": "Appamāṇo buddho,",
"ja203:9.2": "Appamāṇo dhammo;",
"ja203:9.3": "Appamāṇo saṃgho,",
"ja203:9.4": "Pamāṇavantāni sarīsapāni;",
"ja203:9.5": "Ahivicchikasatapadī,",
"ja203:9.6": "Uṇṇanābhi sarabū mūsikā.",
"ja203:10.1": "Katā me rakkhā katā me parittā,",
"ja203:10.2": "Paṭikkamantu bhūtāni;",
"ja203:10.3": "Sohaṃ namo bhagavato,",
"ja203:10.4": "Namo sattannaṃ sammāsambuddhānan”ti.",
"ja203:11.1": "Khaṇḍajātakaṃ tatiyaṃ."
}