{
"ja430:1.1": "Jātaka",
"ja430:2.1": "Navakanipāta",
"ja430:3.1": "Gijjhavagga",
"ja430:4.1": "4. Cūḷasuvajātaka",
"ja430:5.1": "“Santi rukkhā haripattā,",
"ja430:5.2": "dumā nekaphalā bahū;",
"ja430:5.3": "Kasmā nu sukkhe koḷāpe,",
"ja430:5.4": "suvassa nirato mano”.",
"ja430:6.1": "“Phalassa upabhuñjimhā,",
"ja430:6.2": "nekavassagaṇe bahū;",
"ja430:6.3": "Aphalampi viditvāna,",
"ja430:6.4": "sāva metti yathā pure”.",
"ja430:7.1": "“Sukkhañca rukkhaṃ koḷāpaṃ,",
"ja430:7.2": "opattamaphalaṃ dumaṃ;",
"ja430:7.3": "Ohāya sakuṇā yanti,",
"ja430:7.4": "kiṃ dosaṃ passase dija”.",
"ja430:8.1": "“Ye phalatthā sambhajanti,",
"ja430:8.2": "aphaloti jahanti naṃ;",
"ja430:8.3": "Attatthapaññā dummedhā,",
"ja430:8.4": "te honti pakkhapātino”.",
"ja430:9.1": "“Sādhu sakkhi kataṃ hoti,",
"ja430:9.2": "metti saṃsati santhavo;",
"ja430:9.3": "Sacetaṃ dhammaṃ rocesi,",
"ja430:9.4": "pāsaṃsosi vijānataṃ.",
"ja430:10.1": "So te suva varaṃ dammi,",
"ja430:10.2": "pattayāna vihaṅgama;",
"ja430:10.3": "Varaṃ varassu vakkaṅga,",
"ja430:10.4": "yaṃ kiñci manasicchasi”.",
"ja430:11.1": "“Api nāma naṃ passeyyaṃ,",
"ja430:11.2": "sapattaṃ saphalaṃ dumaṃ;",
"ja430:11.3": "Daliddova nidhiṃ laddhā,",
"ja430:11.4": "nandeyyāhaṃ punappunaṃ”.",
"ja430:12.1": "“Tato amatamādāya,",
"ja430:12.2": "abhisiñci mahīruhaṃ;",
"ja430:12.3": "Tassa sākhā virūhiṃsu,",
"ja430:12.4": "sītacchāyā manoramā”.",
"ja430:13.1": "“Evaṃ sakka sukhī hohi,",
"ja430:13.2": "saha sabbehi ñātibhi;",
"ja430:13.3": "Yathāhamajja sukhito,",
"ja430:13.4": "disvāna saphalaṃ dumaṃ.",
"ja430:14.1": "Suvassa ca varaṃ datvā,",
"ja430:14.2": "katvāna saphalaṃ dumaṃ;",
"ja430:14.3": "Pakkāmi saha bhariyāya,",
"ja430:14.4": "devānaṃ nandanaṃ vanan”ti.",
"ja430:15.1": "Cūḷasuvajātakaṃ catutthaṃ."
}