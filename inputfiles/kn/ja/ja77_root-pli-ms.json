{
"ja77:1.1": "Jātaka",
"ja77:2.1": "Ekakanipāta",
"ja77:3.1": "Varuṇavagga",
"ja77:4.1": "7. Mahāsupinajātaka",
"ja77:5.1": "“Usabhā rukkhā gāviyo gavā ca,",
"ja77:5.2": "Asso kaṃso siṅgālī ca kumbho;",
"ja77:5.3": "Pokkharaṇī ca apākacandanaṃ,",
"ja77:5.4": "Lābūni sīdanti silā plavan”ti.",
"ja77:6.1": "Maṇḍūkiyo kaṇhasappe gilanti,",
"ja77:6.2": "Kākaṃ supaṇṇā parivārayanti;",
"ja77:6.3": "Tasā vakā eḷakānaṃ bhayāhi,",
"ja77:6.4": "Vipariyāso vattati nayidha matthīti.",
"ja77:7.1": "Mahāsupinajātakaṃ sattamaṃ."
}