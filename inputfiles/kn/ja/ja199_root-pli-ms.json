{
"ja199:1.1": "Jātaka",
"ja199:2.1": "Dukanipāta",
"ja199:3.1": "Ruhakavagga",
"ja199:4.1": "9. Gahapatijātaka",
"ja199:5.1": "“Ubhayaṃ me na khamati,",
"ja199:5.2": "ubhayaṃ me na ruccati;",
"ja199:5.3": "Yācāyaṃ koṭṭhamotiṇṇā,",
"ja199:5.4": "nāddasaṃ iti bhāsati.",
"ja199:6.1": "Taṃ taṃ gāmapati brūmi,",
"ja199:6.2": "kadare appasmi jīvite;",
"ja199:6.3": "Dve māse saṅgaraṃ katvā,",
"ja199:6.4": "maṃsaṃ jaraggavaṃ kisaṃ;",
"ja199:6.5": "Appattakāle codesi,",
"ja199:6.6": "tampi mayhaṃ na ruccatī”ti.",
"ja199:7.1": "Gahapatijātakaṃ navamaṃ."
}