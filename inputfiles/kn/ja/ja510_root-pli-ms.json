{
"ja510:1.1": "Jātaka",
"ja510:2.1": "Vīsatinipāta",
"ja510:3.1": "Mātaṅgavagga",
"ja510:4.1": "14. Ayogharajātaka",
"ja510:5.1": "“Yamekarattiṃ paṭhamaṃ,",
"ja510:5.2": "gabbhe vasati māṇavo;",
"ja510:5.3": "Abbhuṭṭhitova so yāti,",
"ja510:5.4": "sagacchaṃ na nivattati.",
"ja510:6.1": "Na yujjhamānā na balenavassitā,",
"ja510:6.2": "Narā na jīranti na cāpi miyyare;",
"ja510:6.3": "Sabbaṃ hidaṃ jātijarāyupaddutaṃ,",
"ja510:6.4": "Taṃ me matī hoti carāmi dhammaṃ.",
"ja510:7.1": "Caturaṅginiṃ senaṃ subhiṃsarūpaṃ,",
"ja510:7.2": "Jayanti raṭṭhādhipatī pasayha;",
"ja510:7.3": "Na maccuno jayitumussahanti,",
"ja510:7.4": "Taṃ me matī hoti carāmi dhammaṃ.",
"ja510:8.1": "Hatthīhi assehi rathehi pattibhi,",
"ja510:8.2": "Parivāritā muccare ekacceyyā;",
"ja510:8.3": "Na maccuno muccitumussahanti,",
"ja510:8.4": "Taṃ me matī hoti carāmi dhammaṃ.",
"ja510:9.1": "Hatthīhi assehi rathehi pattibhi,",
"ja510:9.2": "Sūrā pabhañjanti padhaṃsayanti;",
"ja510:9.3": "Na maccuno bhañjitumussahanti,",
"ja510:9.4": "Taṃ me matī hoti carāmi dhammaṃ.",
"ja510:10.1": "Mattā gajā bhinnagaḷā pabhinnā,",
"ja510:10.2": "Nagarāni maddanti janaṃ hananti;",
"ja510:10.3": "Na maccuno madditumussahanti,",
"ja510:10.4": "Taṃ me matī hoti carāmi dhammaṃ.",
"ja510:11.1": "Issāsino katahatthāpi vīrā,",
"ja510:11.2": "Dūrepātī akkhaṇavedhinopi;",
"ja510:11.3": "Na maccuno vijjhitumussahanti,",
"ja510:11.4": "Taṃ me matī hoti carāmi dhammaṃ.",
"ja510:12.1": "Sarāni khīyanti saselakānanā,",
"ja510:12.2": "Sabbaṃ hidaṃ khīyati dīghamantaraṃ;",
"ja510:12.3": "Sabbaṃ hidaṃ bhañjare kālapariyāyaṃ,",
"ja510:12.4": "Taṃ me matī hoti carāmi dhammaṃ.",
"ja510:13.1": "Sabbesa mevaṃ hi narāna nārinaṃ,",
"ja510:13.2": "Calācalaṃ pāṇabhunodha jīvitaṃ;",
"ja510:13.3": "Paṭova dhuttassa dumova kūlajo,",
"ja510:13.4": "Taṃ me matī hoti carāmi dhammaṃ.",
"ja510:14.1": "Dumapphalāneva patanti māṇavā,",
"ja510:14.2": "Daharā ca vuddhā ca sarīrabhedā;",
"ja510:14.3": "Nāriyo narā majjhimaporisā ca,",
"ja510:14.4": "Taṃ me matī hoti carāmi dhammaṃ.",
"ja510:15.1": "Nāyaṃ vayo tārakarājasannibho,",
"ja510:15.2": "Yadabbhatītaṃ gatameva dāni taṃ;",
"ja510:15.3": "Jiṇṇassa hī natthi ratī kuto sukhaṃ,",
"ja510:15.4": "Taṃ me matī hoti carāmi dhammaṃ.",
"ja510:16.1": "Yakkhā pisācā athavāpi petā,",
"ja510:16.2": "Kupitā te assasanti manusse;",
"ja510:16.3": "Na maccuno assasitussahanti,",
"ja510:16.4": "Taṃ me matī hoti carāmi dhammaṃ.",
"ja510:17.1": "Yakkhe pisāce athavāpi pete,",
"ja510:17.2": "Kupitepi te nijjhapanaṃ karonti;",
"ja510:17.3": "Na maccuno nijjhapanaṃ karonti,",
"ja510:17.4": "Taṃ me matī hoti carāmi dhammaṃ.",
"ja510:18.1": "Aparādhake dūsake heṭhake ca,",
"ja510:18.2": "Rājāno daṇḍenti viditvāna dosaṃ;",
"ja510:18.3": "Na maccuno daṇḍayitussahanti,",
"ja510:18.4": "Taṃ me matī hoti carāmi dhammaṃ.",
"ja510:19.1": "Aparādhakā dūsakā heṭhakā ca,",
"ja510:19.2": "Labhanti te rājino nijjhapetuṃ;",
"ja510:19.3": "Na maccuno nijjhapanaṃ karonti,",
"ja510:19.4": "Taṃ me matī hoti carāmi dhammaṃ.",
"ja510:20.1": "Na khattiyoti na ca brāhmaṇoti,",
"ja510:20.2": "Na aḍḍhakā balavā tejavāpi;",
"ja510:20.3": "Na maccurājassa apekkhamatthi,",
"ja510:20.4": "Taṃ me matī hoti carāmi dhammaṃ.",
"ja510:21.1": "Sīhā ca byagghā ca athopi dīpiyo,",
"ja510:21.2": "Pasayha khādanti vipphandamānaṃ;",
"ja510:21.3": "Na maccuno khāditumussahanti,",
"ja510:21.4": "Taṃ me matī hoti carāmi dhammaṃ.",
"ja510:22.1": "Māyākārā raṅgamajjhe karontā,",
"ja510:22.2": "Mohenti cakkhūni janassa tāvade;",
"ja510:22.3": "Na maccuno mohayitussahanti,",
"ja510:22.4": "Taṃ me matī hoti carāmi dhammaṃ.",
"ja510:23.1": "Āsīvisā kupitā uggatejā,",
"ja510:23.2": "Ḍaṃsanti mārentipi te manusse;",
"ja510:23.3": "Na maccuno ḍaṃsitumussahanti,",
"ja510:23.4": "Taṃ me matī hoti carāmi dhammaṃ.",
"ja510:24.1": "Āsīvisā kupitā yaṃ ḍaṃsanti,",
"ja510:24.2": "Tikicchakā tesa visaṃ hananti;",
"ja510:24.3": "Na maccuno daṭṭhavisaṃ hananti,",
"ja510:24.4": "Taṃ me matī hoti carāmi dhammaṃ.",
"ja510:25.1": "Dhammantarī vettaraṇī ca bhojo,",
"ja510:25.2": "Visāni hantvāna bhujaṅgamānaṃ;",
"ja510:25.3": "Suyyanti te kālakatā tatheva,",
"ja510:25.4": "Taṃ me matī hoti carāmi dhammaṃ.",
"ja510:26.1": "Vijjādharā ghoramadhīyamānā,",
"ja510:26.2": "Adassanaṃ osadhehi vajanti;",
"ja510:26.3": "Na maccurājassa vajantadassanaṃ,",
"ja510:26.4": "Taṃ me matī hoti carāmi dhammaṃ.",
"ja510:27.1": "Dhammo have rakkhati dhammacāriṃ,",
"ja510:27.2": "Dhammo suciṇṇo sukhamāvahāti;",
"ja510:27.3": "Esānisaṃso dhamme suciṇṇe,",
"ja510:27.4": "Na duggatiṃ gacchati dhammacārī.",
"ja510:28.1": "Na hi dhammo adhammo ca,",
"ja510:28.2": "Ubho samavipākino;",
"ja510:28.3": "Adhammo nirayaṃ neti,",
"ja510:28.4": "Dhammo pāpeti suggatin”ti. #2605",
"ja510:29.1": "Ayogharajātakaṃ cuddasamaṃ.",
"ja510:30.1": "Vīsatinipātaṃ niṭṭhitaṃ.",
"ja510:31.1": "Tassuddānaṃ",
"ja510:32.1": "Mātaṅga sambhūta sivi sirimanto,",
"ja510:32.2": "Rohaṇa haṃsa sattigumbo bhallātiya;",
"ja510:32.3": "Somanassa campeyya brahma pañca-",
"ja510:32.4": "Paṇḍita cirassaṃvata ayogharāti."
}