{
  "ja325:0.1": "Jātaka ",
  "ja325:0.2": "Catukkanipāta ",
  "ja325:0.3": "Kuṭidūsakavagga ",
  "ja325:0.4": "5. Godharājajātaka ",
  "ja325:1.1": "“Samaṇaṁ taṁ maññamāno, ",
  "ja325:1.2": "upagacchimasaññataṁ; ",
  "ja325:1.3": "So maṁ daṇḍena pāhāsi, ",
  "ja325:1.4": "yathā assamaṇo tathā”. ",
  "ja325:2.1": "“Kiṁ te jaṭāhi dummedha, ",
  "ja325:2.2": "kiṁ te ajinasāṭiyā; ",
  "ja325:2.3": "Abbhantaraṁ te gahanaṁ, ",
  "ja325:2.4": "bāhiraṁ parimajjasi”. ",
  "ja325:3.1": "“Ehi godha nivattassu, ",
  "ja325:3.2": "bhuñja sālīnamodanaṁ; ",
  "ja325:3.3": "Telaṁ loṇañca me atthi, ",
  "ja325:3.4": "pahūtaṁ mayha pipphali. ",
  "ja325:4.1": "Esa bhiyyo pavekkhāmi, ",
  "ja325:4.2": "vammikaṁ sataporisaṁ; ",
  "ja325:4.3": "Telaṁ loṇañca kittesi, ",
  "ja325:4.4": "ahitaṁ mayha pipphalī”ti. ",
  "ja325:5.1": "Godharājajātakaṁ pañcamaṁ. "
}