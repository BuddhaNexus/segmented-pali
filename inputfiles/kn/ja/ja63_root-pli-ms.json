{
  "ja63:0.1": "Jātaka ",
  "ja63:0.2": "Ekakanipāta ",
  "ja63:0.3": "Itthivagga ",
  "ja63:0.4": "3. Takkapaṇḍitajātaka ",
  "ja63:1.1": "“Kodhanā akataññū ca, ",
  "ja63:1.2": "pisuṇā mittabhedikā; ",
  "ja63:1.3": "Brahmacariyaṁ cara bhikkhu, ",
  "ja63:1.4": "so sukhaṁ na vihāhasīti. ",
  "ja63:2.1": "Takkapaṇḍitajātakaṁ tatiyaṁ. "
}