{
"ja63:1.1": "Jātaka",
"ja63:2.1": "Ekakanipāta",
"ja63:3.1": "Itthivagga",
"ja63:4.1": "3. Takkapaṇḍitajātaka",
"ja63:5.1": "“Kodhanā akataññū ca,",
"ja63:5.2": "pisuṇā mittabhedikā;",
"ja63:5.3": "Brahmacariyaṃ cara bhikkhu,",
"ja63:5.4": "so sukhaṃ na vihāhasīti.",
"ja63:6.1": "Takkapaṇḍitajātakaṃ tatiyaṃ."
}