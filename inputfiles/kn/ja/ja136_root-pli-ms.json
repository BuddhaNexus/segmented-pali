{
"ja136:1.1": "Jātaka",
"ja136:2.1": "Ekakanipāta",
"ja136:3.1": "Asampadānavagga",
"ja136:4.1": "6. Suvaṇṇahaṃsajātaka",
"ja136:5.1": "“Yaṃ laddhaṃ tena tuṭṭhabbaṃ,",
"ja136:5.2": "Atilobho hi pāpako;",
"ja136:5.3": "Haṃsarājaṃ gahetvāna,",
"ja136:5.4": "Suvaṇṇā parihāyathā”ti.",
"ja136:6.1": "Suvaṇṇahaṃsajātakaṃ chaṭṭhaṃ."
}