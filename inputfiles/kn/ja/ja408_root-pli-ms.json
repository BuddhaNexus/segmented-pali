{
  "ja408:0.1": "Jātaka ",
  "ja408:0.2": "Sattakanipāta ",
  "ja408:0.3": "Gandhāravagga ",
  "ja408:0.4": "3. Kumbhakārajātaka ",
  "ja408:1.1": "“Ambāhamaddaṁ vanamantarasmiṁ, ",
  "ja408:1.2": "Nīlobhāsaṁ phalitaṁ saṁvirūḷhaṁ; ",
  "ja408:1.3": "Tamaddasaṁ phalahetu vibhaggaṁ, ",
  "ja408:1.4": "Taṁ disvā bhikkhācariyaṁ carāmi. ",
  "ja408:2.1": "Selaṁ sumaṭṭhaṁ naravīraniṭṭhitaṁ, ",
  "ja408:2.2": "Nārī yugaṁ dhārayi appasaddaṁ; ",
  "ja408:2.3": "Dutiyañca āgamma ahosi saddo, ",
  "ja408:2.4": "Taṁ disvā bhikkhācariyaṁ carāmi. ",
  "ja408:3.1": "Dijā dijaṁ kuṇapamāharantaṁ, ",
  "ja408:3.2": "Ekaṁ samānaṁ bahukā samecca; ",
  "ja408:3.3": "Āhārahetū paripātayiṁsu, ",
  "ja408:3.4": "Taṁ disvā bhikkhācariyaṁ carāmi. ",
  "ja408:4.1": "Usabhāhamaddaṁ yūthassa majjhe, ",
  "ja408:4.2": "Calakkakuṁ vaṇṇabalūpapannaṁ; ",
  "ja408:4.3": "Tamaddasaṁ kāmahetu vitunnaṁ, ",
  "ja408:4.4": "Taṁ disvā bhikkhācariyaṁ carāmi”. ",
  "ja408:5.1": "“Karaṇḍako kaliṅgānaṁ, ",
  "ja408:5.2": "gandhārānañca naggaji; ",
  "ja408:5.3": "Nimirājā videhānaṁ, ",
  "ja408:5.4": "pañcālānañca dummukho; ",
  "ja408:5.5": "Ete raṭṭhāni hitvāna, ",
  "ja408:5.6": "pabbajiṁsu akiñcanā. ",
  "ja408:6.1": "Sabbepime devasamā samāgatā, ",
  "ja408:6.2": "Aggī yathā pajjalito tathevime; ",
  "ja408:6.3": "Ahampi eko carissāmi bhaggavi, ",
  "ja408:6.4": "Hitvāna kāmāni yathodhikāni”. ",
  "ja408:7.1": "“Ayameva kālo na hi añño atthi, ",
  "ja408:7.2": "Anusāsitā me na bhaveyya pacchā; ",
  "ja408:7.3": "Ahampi ekā carissāmi bhaggava, ",
  "ja408:7.4": "Sakuṇīva muttā purisassa hatthā”. ",
  "ja408:8.1": "“Āmaṁ pakkañca jānanti, ",
  "ja408:8.2": "atho loṇaṁ aloṇakaṁ; ",
  "ja408:8.3": "Tamahaṁ disvāna pabbajiṁ, ",
  "ja408:8.4": "careva tvaṁ carāmahan”ti. ",
  "ja408:9.1": "Kumbhakārajātakaṁ tatiyaṁ. "
}