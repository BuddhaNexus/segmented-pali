{
"ja408:1.1": "Jātaka",
"ja408:2.1": "Sattakanipāta",
"ja408:3.1": "Gandhāravagga",
"ja408:4.1": "3. Kumbhakārajātaka",
"ja408:5.1": "“Ambāhamaddaṃ vanamantarasmiṃ,",
"ja408:5.2": "Nīlobhāsaṃ phalitaṃ saṃvirūḷhaṃ;",
"ja408:5.3": "Tamaddasaṃ phalahetu vibhaggaṃ,",
"ja408:5.4": "Taṃ disvā bhikkhācariyaṃ carāmi.",
"ja408:6.1": "Selaṃ sumaṭṭhaṃ naravīraniṭṭhitaṃ,",
"ja408:6.2": "Nārī yugaṃ dhārayi appasaddaṃ;",
"ja408:6.3": "Dutiyañca āgamma ahosi saddo,",
"ja408:6.4": "Taṃ disvā bhikkhācariyaṃ carāmi.",
"ja408:7.1": "Dijā dijaṃ kuṇapamāharantaṃ,",
"ja408:7.2": "Ekaṃ samānaṃ bahukā samecca;",
"ja408:7.3": "Āhārahetū paripātayiṃsu,",
"ja408:7.4": "Taṃ disvā bhikkhācariyaṃ carāmi.",
"ja408:8.1": "Usabhāhamaddaṃ yūthassa majjhe,",
"ja408:8.2": "Calakkakuṃ vaṇṇabalūpapannaṃ;",
"ja408:8.3": "Tamaddasaṃ kāmahetu vitunnaṃ,",
"ja408:8.4": "Taṃ disvā bhikkhācariyaṃ carāmi”.",
"ja408:9.1": "“Karaṇḍako kaliṅgānaṃ,",
"ja408:9.2": "gandhārānañca naggaji;",
"ja408:9.3": "Nimirājā videhānaṃ,",
"ja408:9.4": "pañcālānañca dummukho;",
"ja408:9.5": "Ete raṭṭhāni hitvāna,",
"ja408:9.6": "pabbajiṃsu akiñcanā.",
"ja408:10.1": "Sabbepime devasamā samāgatā,",
"ja408:10.2": "Aggī yathā pajjalito tathevime;",
"ja408:10.3": "Ahampi eko carissāmi bhaggavi,",
"ja408:10.4": "Hitvāna kāmāni yathodhikāni”.",
"ja408:11.1": "“Ayameva kālo na hi añño atthi,",
"ja408:11.2": "Anusāsitā me na bhaveyya pacchā;",
"ja408:11.3": "Ahampi ekā carissāmi bhaggava,",
"ja408:11.4": "Sakuṇīva muttā purisassa hatthā”.",
"ja408:12.1": "“Āmaṃ pakkañca jānanti,",
"ja408:12.2": "atho loṇaṃ aloṇakaṃ;",
"ja408:12.3": "Tamahaṃ disvāna pabbajiṃ,",
"ja408:12.4": "careva tvaṃ carāmahan”ti.",
"ja408:13.1": "Kumbhakārajātakaṃ tatiyaṃ."
}