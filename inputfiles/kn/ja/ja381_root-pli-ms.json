{
"ja381:1.1": "Jātaka",
"ja381:2.1": "Chakkanipāta",
"ja381:3.1": "Avāriyavagga",
"ja381:4.1": "6. Migālopajātaka",
"ja381:5.1": "“Na me rucci migālopa,",
"ja381:5.2": "yassa te tādisī gatī;",
"ja381:5.3": "Atuccaṃ tāta patasi,",
"ja381:5.4": "abhūmiṃ tāta sevasi.",
"ja381:6.1": "Catukkaṇṇaṃva kedāraṃ,",
"ja381:6.2": "yadā te pathavī siyā;",
"ja381:6.3": "Tato tāta nivattassu,",
"ja381:6.4": "māssu etto paraṃ gami”.",
"ja381:7.1": "“Santi aññepi sakuṇā,",
"ja381:7.2": "pattayānā vihaṅgamā;",
"ja381:7.3": "Akkhittā vātavegena,",
"ja381:7.4": "naṭṭhā te sassatīsamā.",
"ja381:8.1": "Akatvā apanandassa,",
"ja381:8.2": "pitu vuddhassa sāsanaṃ;",
"ja381:8.3": "Kālavāte atikkamma,",
"ja381:8.4": "verambhānaṃ vasaṃ agā.",
"ja381:9.1": "Tassa puttā ca dārā ca,",
"ja381:9.2": "ye caññe anujīvino;",
"ja381:9.3": "Sabbe byasanamāpāduṃ,",
"ja381:9.4": "anovādakare dije.",
"ja381:10.1": "Evampi idha vuddhānaṃ,",
"ja381:10.2": "yo vākyaṃ nāvabujjhati;",
"ja381:10.3": "Atisīmacaro ditto,",
"ja381:10.4": "gijjhovātītasāsano;",
"ja381:10.5": "Sabbe byasanaṃ papponti,",
"ja381:10.6": "akatvā vuddhasāsanan”ti.",
"ja381:11.1": "Migālopajātakaṃ chaṭṭhaṃ."
}