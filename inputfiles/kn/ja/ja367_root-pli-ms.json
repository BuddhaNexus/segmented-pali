{
"ja367:1.1": "Jātaka",
"ja367:2.1": "Pañcakanipāta",
"ja367:3.1": "Vaṇṇārohavagga",
"ja367:4.1": "7. Sāḷiyajātaka",
"ja367:5.1": "“Yvāyaṃ sāḷiyachāpoti,",
"ja367:5.2": "kaṇhasappaṃ agāhayi;",
"ja367:5.3": "Tena sappenayaṃ daṭṭho,",
"ja367:5.4": "hato pāpānusāsako.",
"ja367:6.1": "Ahantāra mahantāraṃ,",
"ja367:6.2": "yo naro hantumicchati;",
"ja367:6.3": "Evaṃ so nihato seti,",
"ja367:6.4": "yathāyaṃ puriso hato.",
"ja367:7.1": "Ahantāra maghātentaṃ,",
"ja367:7.2": "yo naro hantumicchati;",
"ja367:7.3": "Evaṃ so nihato seti,",
"ja367:7.4": "yathāyaṃ puriso hato.",
"ja367:8.1": "Yathā paṃsumuṭṭhiṃ puriso,",
"ja367:8.2": "paṭivātaṃ paṭikkhipe;",
"ja367:8.3": "Tameva so rajo hanti,",
"ja367:8.4": "tathāyaṃ puriso hato.",
"ja367:9.1": "Yo appaduṭṭhassa narassa dussati,",
"ja367:9.2": "Suddhassa posassa anaṅgaṇassa;",
"ja367:9.3": "Tameva bālaṃ pacceti pāpaṃ,",
"ja367:9.4": "Sukhumo rajo paṭivātaṃva khitto”ti.",
"ja367:10.1": "Sāḷiyajātakaṃ sattamaṃ."
}