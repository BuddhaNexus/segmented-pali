{
"ja224:1.1": "Jātaka",
"ja224:2.1": "Dukanipāta",
"ja224:3.1": "Kāsāvavagga",
"ja224:4.1": "4. Kumbhilajātaka",
"ja224:5.1": "“Yassete caturo dhammā,",
"ja224:5.2": "vānarinda yathā tava;",
"ja224:5.3": "Saccaṃ dhammo dhiti cāgo,",
"ja224:5.4": "diṭṭhaṃ so ativattati.",
"ja224:6.1": "Yassa cete na vijjanti,",
"ja224:6.2": "guṇā paramabhaddakā;",
"ja224:6.3": "Saccaṃ dhammo dhiti cāgo,",
"ja224:6.4": "diṭṭhaṃ so nātivattatī”ti.",
"ja224:7.1": "Kumbhilajātakaṃ catutthaṃ."
}