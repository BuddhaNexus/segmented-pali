{
  "ja227:0.1": "Jātaka ",
  "ja227:0.2": "Dukanipāta ",
  "ja227:0.3": "Kāsāvavagga ",
  "ja227:0.4": "7. Gūthapāṇajātaka ",
  "ja227:1.1": "“Sūro sūrena saṅgamma, ",
  "ja227:1.2": "vikkantena pahārinā; ",
  "ja227:1.3": "Ehi nāga nivattassu, ",
  "ja227:1.4": "kiṁ nu bhīto palāyasi; ",
  "ja227:1.5": "Passantu aṅgamagadhā, ",
  "ja227:1.6": "mama tuyhañca vikkamaṁ”. ",
  "ja227:2.1": "“Na taṁ pādā vadhissāmi, ",
  "ja227:2.2": "na dantehi na soṇḍiyā; ",
  "ja227:2.3": "Mīḷhena taṁ vadhissāmi, ",
  "ja227:2.4": "pūti haññatu pūtinā”ti. ",
  "ja227:3.1": "Gūthapāṇajātakaṁ sattamaṁ. "
}