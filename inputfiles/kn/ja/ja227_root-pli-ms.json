{
"ja227:1.1": "Jātaka",
"ja227:2.1": "Dukanipāta",
"ja227:3.1": "Kāsāvavagga",
"ja227:4.1": "7. Gūthapāṇajātaka",
"ja227:5.1": "“Sūro sūrena saṅgamma,",
"ja227:5.2": "vikkantena pahārinā;",
"ja227:5.3": "Ehi nāga nivattassu,",
"ja227:5.4": "kiṃ nu bhīto palāyasi;",
"ja227:5.5": "Passantu aṅgamagadhā,",
"ja227:5.6": "mama tuyhañca vikkamaṃ”.",
"ja227:6.1": "“Na taṃ pādā vadhissāmi,",
"ja227:6.2": "na dantehi na soṇḍiyā;",
"ja227:6.3": "Mīḷhena taṃ vadhissāmi,",
"ja227:6.4": "pūti haññatu pūtinā”ti.",
"ja227:7.1": "Gūthapāṇajātakaṃ sattamaṃ."
}