{
"ja316:1.1": "Jātaka",
"ja316:2.1": "Catukkanipāta",
"ja316:3.1": "Pucimandavagga",
"ja316:4.1": "6. Sasapaṇḍitajātaka",
"ja316:5.1": "“Satta me rohitā macchā,",
"ja316:5.2": "udakā thalamubbhatā;",
"ja316:5.3": "Idaṃ brāhmaṇa me atthi,",
"ja316:5.4": "etaṃ bhutvā vane vasa”.",
"ja316:6.1": "“Dussa me khettapālassa,",
"ja316:6.2": "rattibhattaṃ apābhataṃ;",
"ja316:6.3": "Maṃsasūlā ca dve godhā,",
"ja316:6.4": "ekañca dadhivārakaṃ;",
"ja316:6.5": "Idaṃ brāhmaṇa me atthi,",
"ja316:6.6": "etaṃ bhutvā vane vasa”.",
"ja316:7.1": "“Ambapakkaṃ dakaṃ sītaṃ,",
"ja316:7.2": "Sītacchāyā manoramā;",
"ja316:7.3": "Idaṃ brāhmaṇa me atthi,",
"ja316:7.4": "Etaṃ bhutvā vane vasa”.",
"ja316:8.1": "“Na sasassa tilā atthi,",
"ja316:8.2": "Na muggā napi taṇḍulā;",
"ja316:8.3": "Iminā agginā pakkaṃ,",
"ja316:8.4": "Mamaṃ bhutvā vane vasā”ti.",
"ja316:9.1": "Sasapaṇḍitajātakaṃ chaṭṭhaṃ."
}