{
"ja279:1.1": "Jātaka",
"ja279:2.1": "Tikanipāta",
"ja279:3.1": "Udapānavagga",
"ja279:4.1": "9. Satapattajātaka",
"ja279:5.1": "“Yathā māṇavako panthe,",
"ja279:5.2": "siṅgāliṃ vanagocariṃ;",
"ja279:5.3": "Atthakāmaṃ pavedentiṃ,",
"ja279:5.4": "anatthakāmāti maññati;",
"ja279:5.5": "Anatthakāmaṃ satapattaṃ,",
"ja279:5.6": "atthakāmoti maññati.",
"ja279:6.1": "Evameva idhekacco,",
"ja279:6.2": "puggalo hoti tādiso;",
"ja279:6.3": "Hitehi vacanaṃ vutto,",
"ja279:6.4": "paṭigaṇhāti vāmato.",
"ja279:7.1": "Ye ca kho naṃ pasaṃsanti,",
"ja279:7.2": "bhayā ukkaṃsayanti vā;",
"ja279:7.3": "Tañhi so maññate mittaṃ,",
"ja279:7.4": "satapattaṃva māṇavo”ti.",
"ja279:8.1": "Satapattajātakaṃ navamaṃ."
}