{
"ja400:1.1": "Jātaka",
"ja400:2.1": "Sattakanipāta",
"ja400:3.1": "Kukkuvagga",
"ja400:4.1": "5. Dabbhapupphajātaka",
"ja400:5.1": "“Anutīracārī bhaddante,",
"ja400:5.2": "sahāyamanudhāva maṃ;",
"ja400:5.3": "Mahā me gahito maccho,",
"ja400:5.4": "so maṃ harati vegasā”.",
"ja400:6.1": "“Gambhīracārī bhaddante,",
"ja400:6.2": "daḷhaṃ gaṇhāhi thāmasā;",
"ja400:6.3": "Ahaṃ taṃ uddharissāmi,",
"ja400:6.4": "supaṇṇo uragāmiva”.",
"ja400:7.1": "“Vivādo no samuppanno,",
"ja400:7.2": "dabbhapuppha suṇohi me;",
"ja400:7.3": "Samehi medhagaṃ samma,",
"ja400:7.4": "vivādo vūpasammataṃ”.",
"ja400:8.1": "“Dhammaṭṭhohaṃ pure āsiṃ,",
"ja400:8.2": "bahū aḍḍā me tīritā;",
"ja400:8.3": "Samemi medhagaṃ samma,",
"ja400:8.4": "vivādo vūpasammataṃ.",
"ja400:9.1": "Anutīracāri naṅguṭṭhaṃ,",
"ja400:9.2": "sīsaṃ gambhīracārino;",
"ja400:9.3": "Accāyaṃ majjhimo khaṇḍo,",
"ja400:9.4": "dhammaṭṭhassa bhavissati”.",
"ja400:10.1": "“Cirampi bhakkho abhavissa,",
"ja400:10.2": "sace na vivademase;",
"ja400:10.3": "Asīsakaṃ anaṅguṭṭhaṃ,",
"ja400:10.4": "siṅgālo harati rohitaṃ”.",
"ja400:11.1": "“Yathāpi rājā nandeyya,",
"ja400:11.2": "rajjaṃ laddhāna khattiyo;",
"ja400:11.3": "Evāhamajja nandāmi,",
"ja400:11.4": "disvā puṇṇamukhaṃ patiṃ.",
"ja400:12.1": "Kathaṃ nu thalajo santo,",
"ja400:12.2": "udake macchaṃ parāmasi;",
"ja400:12.3": "Puṭṭho me samma akkhāhi,",
"ja400:12.4": "kathaṃ adhigataṃ tayā”.",
"ja400:13.1": "“Vivādena kisā honti,",
"ja400:13.2": "vivādena dhanakkhayā;",
"ja400:13.3": "Jīnā uddā vivādena,",
"ja400:13.4": "bhuñja māyāvi rohitaṃ.",
"ja400:14.1": "Evameva manussesu,",
"ja400:14.2": "vivādo yattha jāyati;",
"ja400:14.3": "Dhammaṭṭhaṃ paṭidhāvanti,",
"ja400:14.4": "so hi nesaṃ vināyako;",
"ja400:14.5": "Dhanāpi tattha jīyanti,",
"ja400:14.6": "rājakoso pavaḍḍhatī”ti.",
"ja400:15.1": "Dabbhapupphajātakaṃ pañcamaṃ."
}