{
  "ja400:0.1": "Jātaka ",
  "ja400:0.2": "Sattakanipāta ",
  "ja400:0.3": "Kukkuvagga ",
  "ja400:0.4": "5. Dabbhapupphajātaka ",
  "ja400:1.1": "“Anutīracārī bhaddante, ",
  "ja400:1.2": "sahāyamanudhāva maṁ; ",
  "ja400:1.3": "Mahā me gahito maccho, ",
  "ja400:1.4": "so maṁ harati vegasā”. ",
  "ja400:2.1": "“Gambhīracārī bhaddante, ",
  "ja400:2.2": "daḷhaṁ gaṇhāhi thāmasā; ",
  "ja400:2.3": "Ahaṁ taṁ uddharissāmi, ",
  "ja400:2.4": "supaṇṇo uragāmiva”. ",
  "ja400:3.1": "“Vivādo no samuppanno, ",
  "ja400:3.2": "dabbhapuppha suṇohi me; ",
  "ja400:3.3": "Samehi medhagaṁ samma, ",
  "ja400:3.4": "vivādo vūpasammataṁ”. ",
  "ja400:4.1": "“Dhammaṭṭhohaṁ pure āsiṁ, ",
  "ja400:4.2": "bahū aḍḍā me tīritā; ",
  "ja400:4.3": "Samemi medhagaṁ samma, ",
  "ja400:4.4": "vivādo vūpasammataṁ. ",
  "ja400:5.1": "Anutīracāri naṅguṭṭhaṁ, ",
  "ja400:5.2": "sīsaṁ gambhīracārino; ",
  "ja400:5.3": "Accāyaṁ majjhimo khaṇḍo, ",
  "ja400:5.4": "dhammaṭṭhassa bhavissati”. ",
  "ja400:6.1": "“Cirampi bhakkho abhavissa, ",
  "ja400:6.2": "sace na vivademase; ",
  "ja400:6.3": "Asīsakaṁ anaṅguṭṭhaṁ, ",
  "ja400:6.4": "siṅgālo harati rohitaṁ”. ",
  "ja400:7.1": "“Yathāpi rājā nandeyya, ",
  "ja400:7.2": "rajjaṁ laddhāna khattiyo; ",
  "ja400:7.3": "Evāhamajja nandāmi, ",
  "ja400:7.4": "disvā puṇṇamukhaṁ patiṁ. ",
  "ja400:8.1": "Kathaṁ nu thalajo santo, ",
  "ja400:8.2": "udake macchaṁ parāmasi; ",
  "ja400:8.3": "Puṭṭho me samma akkhāhi, ",
  "ja400:8.4": "kathaṁ adhigataṁ tayā”. ",
  "ja400:9.1": "“Vivādena kisā honti, ",
  "ja400:9.2": "vivādena dhanakkhayā; ",
  "ja400:9.3": "Jīnā uddā vivādena, ",
  "ja400:9.4": "bhuñja māyāvi rohitaṁ. ",
  "ja400:10.1": "Evameva manussesu, ",
  "ja400:10.2": "vivādo yattha jāyati; ",
  "ja400:10.3": "Dhammaṭṭhaṁ paṭidhāvanti, ",
  "ja400:10.4": "so hi nesaṁ vināyako; ",
  "ja400:10.5": "Dhanāpi tattha jīyanti, ",
  "ja400:10.6": "rājakoso pavaḍḍhatī”ti. ",
  "ja400:11.1": "Dabbhapupphajātakaṁ pañcamaṁ. "
}