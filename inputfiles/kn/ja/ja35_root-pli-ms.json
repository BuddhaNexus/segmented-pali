{
"ja35:1.1": "Jātaka",
"ja35:2.1": "Ekakanipāta",
"ja35:3.1": "Kulāvakavagga",
"ja35:4.1": "5. Vaṭṭakajātaka",
"ja35:5.1": "“Santi pakkhā apatanā,",
"ja35:5.2": "santi pādā avañcanā;",
"ja35:5.3": "Mātāpitā ca nikkhantā,",
"ja35:5.4": "jātaveda paṭikkamā”ti.",
"ja35:6.1": "Vaṭṭakajātakaṃ pañcamaṃ."
}