{
"ja129:1.1": "Jātaka",
"ja129:2.1": "Ekakanipāta",
"ja129:3.1": "Kusanāḷivagga",
"ja129:4.1": "9. Aggikabhāradvājajātaka",
"ja129:5.1": "“Nāyaṃ sikhā puññahetu,",
"ja129:5.2": "ghāsahetu ayaṃ sikhā;",
"ja129:5.3": "Nāguṭṭhigaṇanaṃ yāti,",
"ja129:5.4": "alaṃ te hotu aggikā”ti.",
"ja129:6.1": "Aggikabhāradvājajātakaṃ navamaṃ."
}