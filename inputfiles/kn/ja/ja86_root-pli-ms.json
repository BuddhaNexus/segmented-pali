{
"ja86:1.1": "Jātaka",
"ja86:2.1": "Ekakanipāta",
"ja86:3.1": "Apāyimhavagga",
"ja86:4.1": "6. Sīlavīmaṃsakajātaka",
"ja86:5.1": "“Sīlaṃ kireva kalyāṇaṃ,",
"ja86:5.2": "sīlaṃ loke anuttaraṃ;",
"ja86:5.3": "Passa ghoraviso nāgo,",
"ja86:5.4": "sīlavāti na haññatī”ti.",
"ja86:6.1": "Sīlavīmaṃsakajātakaṃ chaṭṭhaṃ."
}