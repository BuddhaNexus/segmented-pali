{
"ja371:1.1": "Jātaka",
"ja371:2.1": "Pañcakanipāta",
"ja371:3.1": "Aḍḍhavagga",
"ja371:4.1": "1. Dīghītikosalajātaka",
"ja371:5.1": "“Evaṃbhūtassa te rāja,",
"ja371:5.2": "āgatassa vase mama;",
"ja371:5.3": "Atthi nu koci pariyāyo,",
"ja371:5.4": "yo taṃ dukkhā pamocaye”.",
"ja371:6.1": "“Evaṃbhūtassa me tāta,",
"ja371:6.2": "āgatassa vase tava;",
"ja371:6.3": "Natthi no koci pariyāyo,",
"ja371:6.4": "yo maṃ dukkhā pamocaye”.",
"ja371:7.1": "“Nāññaṃ sucaritaṃ rāja,",
"ja371:7.2": "nāññaṃ rāja subhāsitaṃ;",
"ja371:7.3": "Tāyate maraṇakāle,",
"ja371:7.4": "evamevitaraṃ dhanaṃ.",
"ja371:8.1": "Akkocchi maṃ avadhi maṃ,",
"ja371:8.2": "ajini maṃ ahāsi me;",
"ja371:8.3": "Ye ca taṃ upanayhanti,",
"ja371:8.4": "veraṃ tesaṃ na sammati.",
"ja371:9.1": "Akkocchi maṃ avadhi maṃ,",
"ja371:9.2": "ajini maṃ ahāsi me;",
"ja371:9.3": "Ye ca taṃ nupanayhanti,",
"ja371:9.4": "veraṃ tesūpasammati.",
"ja371:10.1": "Na hi verena verāni,",
"ja371:10.2": "sammantīdha kudācanaṃ;",
"ja371:10.3": "Averena ca sammanti,",
"ja371:10.4": "esa dhammo sanantano”ti.",
"ja371:11.1": "Dīghītikosalajātakaṃ paṭhamaṃ."
}