{
"ja353:1.1": "Jātaka",
"ja353:2.1": "Pañcakanipāta",
"ja353:3.1": "Maṇikuṇḍalavagga",
"ja353:4.1": "3. Venasākhajātaka",
"ja353:5.1": "“Nayidaṃ niccaṃ bhavitabbaṃ brahmadatta,",
"ja353:5.2": "Khemaṃ subhikkhaṃ sukhatā ca kāye;",
"ja353:5.3": "Atthaccaye mā ahu sampamūḷho,",
"ja353:5.4": "Bhinnaplavo sāgarasseva majjhe.",
"ja353:6.1": "Yāni karoti puriso,",
"ja353:6.2": "tāni attani passati;",
"ja353:6.3": "Kalyāṇakārī kalyāṇaṃ,",
"ja353:6.4": "pāpakārī ca pāpakaṃ;",
"ja353:6.5": "Yādisaṃ vapate bījaṃ,",
"ja353:6.6": "tādisaṃ harate phalaṃ”.",
"ja353:7.1": "“Idaṃ tadācariyavaco,",
"ja353:7.2": "pārāsariyo yadabravi;",
"ja353:7.3": "Mā su tvaṃ akari pāpaṃ,",
"ja353:7.4": "yaṃ tvaṃ pacchā kataṃ tape.",
"ja353:8.1": "Ayameva so piṅgiya venasākho,",
"ja353:8.2": "Yamhi ghātayiṃ khattiyānaṃ sahassaṃ;",
"ja353:8.3": "Alaṅkate candanasārānulitte,",
"ja353:8.4": "Tameva dukkhaṃ paccāgataṃ mamaṃ.",
"ja353:9.1": "Sāmā ca kho candanalittagattā,",
"ja353:9.2": "Laṭṭhīva sobhañjanakassa uggatā;",
"ja353:9.3": "Adisvā kālaṃ karissāmi ubbariṃ,",
"ja353:9.4": "Taṃ me ito dukkhataraṃ bhavissatī”ti.",
"ja353:10.1": "Venasākhajātakaṃ tatiyaṃ."
}