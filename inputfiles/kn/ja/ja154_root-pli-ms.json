{
"ja154:1.1": "Jātaka",
"ja154:2.1": "Dukanipāta",
"ja154:3.1": "Daḷhavagga",
"ja154:4.1": "4. Uragajātaka",
"ja154:5.1": "“Idhūragānaṃ pavaro paviṭṭho,",
"ja154:5.2": "Selassa vaṇṇena pamokkhamicchaṃ;",
"ja154:5.3": "Brahmañca vaṇṇaṃ apacāyamāno,",
"ja154:5.4": "Bubhukkhito no vitarāmi bhottuṃ”.",
"ja154:6.1": "“So brahmagutto cirameva jīva,",
"ja154:6.2": "Dibyā ca te pātubhavantu bhakkhā;",
"ja154:6.3": "Yo brahmavaṇṇaṃ apacāyamāno,",
"ja154:6.4": "Bubhukkhito no vitarāsi bhottun”ti.",
"ja154:7.1": "Uragajātakaṃ catutthaṃ."
}