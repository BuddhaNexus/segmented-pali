{
"ja410:1.1": "Jātaka",
"ja410:2.1": "Sattakanipāta",
"ja410:3.1": "Gandhāravagga",
"ja410:4.1": "5. Somadattajātaka",
"ja410:5.1": "“Yo maṃ pure paccuḍḍeti,",
"ja410:5.2": "araññe dūramāyato;",
"ja410:5.3": "So na dissati mātaṅgo,",
"ja410:5.4": "somadatto kuhiṃ gato.",
"ja410:6.1": "Ayaṃ vā so mato seti,",
"ja410:6.2": "allasiṅgaṃva vacchito;",
"ja410:6.3": "Bhumyā nipatito seti,",
"ja410:6.4": "amarā vata kuñjaro”.",
"ja410:7.1": "“Anagāriyupetassa,",
"ja410:7.2": "vippamuttassa te sato;",
"ja410:7.3": "Samaṇassa na taṃ sādhu,",
"ja410:7.4": "yaṃ petamanusocasi”.",
"ja410:8.1": "“Saṃvāsena have sakka,",
"ja410:8.2": "manussassa migassa vā;",
"ja410:8.3": "Hadaye jāyate pemaṃ,",
"ja410:8.4": "taṃ na sakkā asocituṃ”.",
"ja410:9.1": "“Mataṃ marissaṃ rodanti,",
"ja410:9.2": "ye rudanti lapanti ca;",
"ja410:9.3": "Tasmā tvaṃ isi mā rodi,",
"ja410:9.4": "roditaṃ moghamāhu santo.",
"ja410:10.1": "Kanditena have brahme,",
"ja410:10.2": "mato peto samuṭṭhahe;",
"ja410:10.3": "Sabbe saṅgamma rodāma,",
"ja410:10.4": "aññamaññassa ñātake”.",
"ja410:11.1": "“Ādittaṃ vata maṃ santaṃ,",
"ja410:11.2": "ghatasittaṃva pāvakaṃ;",
"ja410:11.3": "Vārinā viya osiñcaṃ,",
"ja410:11.4": "sabbaṃ nibbāpaye daraṃ.",
"ja410:12.1": "Abbahī vata me sallaṃ,",
"ja410:12.2": "yamāsi hadayassitaṃ;",
"ja410:12.3": "Yo me sokaparetassa,",
"ja410:12.4": "puttasokaṃ apānudi.",
"ja410:13.1": "Sohaṃ abbūḷhasallosmi,",
"ja410:13.2": "vītasoko anāvilo;",
"ja410:13.3": "Na socāmi na rodāmi,",
"ja410:13.4": "tava sutvāna vāsavā”ti.",
"ja410:14.1": "Somadattajātakaṃ pañcamaṃ."
}