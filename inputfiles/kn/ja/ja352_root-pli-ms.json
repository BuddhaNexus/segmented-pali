{
"ja352:1.1": "Jātaka",
"ja352:2.1": "Pañcakanipāta",
"ja352:3.1": "Maṇikuṇḍalavagga",
"ja352:4.1": "2. Sujātajātaka",
"ja352:5.1": "“Kiṃ nu santaramānova,",
"ja352:5.2": "lāyitvā haritaṃ tiṇaṃ;",
"ja352:5.3": "Khāda khādāti lapasi,",
"ja352:5.4": "gatasattaṃ jaraggavaṃ.",
"ja352:6.1": "Na hi annena pānena,",
"ja352:6.2": "mato goṇo samuṭṭhahe;",
"ja352:6.3": "Tvañca tucchaṃ vilapasi,",
"ja352:6.4": "yathā taṃ dummatī tathā”.",
"ja352:7.1": "“Tatheva tiṭṭhati sīsaṃ,",
"ja352:7.2": "hatthapādā ca vāladhi;",
"ja352:7.3": "Sotā tatheva tiṭṭhanti,",
"ja352:7.4": "maññe goṇo samuṭṭhahe.",
"ja352:8.1": "Nevayyakassa sīsañca,",
"ja352:8.2": "hatthapādā ca dissare;",
"ja352:8.3": "Rudaṃ mattikathūpasmiṃ,",
"ja352:8.4": "nanu tvaññeva dummati”.",
"ja352:9.1": "“Ādittaṃ vata maṃ santaṃ,",
"ja352:9.2": "ghatasittaṃva pāvakaṃ;",
"ja352:9.3": "Vārinā viya osiñcaṃ,",
"ja352:9.4": "sabbaṃ nibbāpaye daraṃ.",
"ja352:10.1": "Abbahī vata me sallaṃ,",
"ja352:10.2": "yamāsi hadayassitaṃ;",
"ja352:10.3": "Yo me sokaparetassa,",
"ja352:10.4": "pitusokaṃ apānudi.",
"ja352:11.1": "Sohaṃ abbūḷhasallosmi,",
"ja352:11.2": "vītasoko anāvilo;",
"ja352:11.3": "Na socāmi na rodāmi,",
"ja352:11.4": "tava sutvāna māṇava.",
"ja352:12.1": "Evaṃ karonti sappaññā,",
"ja352:12.2": "ye honti anukampakā;",
"ja352:12.3": "Vinivattenti sokamhā,",
"ja352:12.4": "sujāto pitaraṃ yathā”ti.",
"ja352:13.1": "Sujātajātakaṃ dutiyaṃ."
}