{
"ja416:1.1": "Jātaka",
"ja416:2.1": "Sattakanipāta",
"ja416:3.1": "Gandhāravagga",
"ja416:4.1": "11. Parantapajātaka",
"ja416:5.1": "“Āgamissati me pāpaṃ,",
"ja416:5.2": "āgamissati me bhayaṃ;",
"ja416:5.3": "Tadā hi calitā sākhā,",
"ja416:5.4": "manussena migena vā”.",
"ja416:6.1": "“Bhīruyā nūna me kāmo,",
"ja416:6.2": "avidūre vasantiyā;",
"ja416:6.3": "Karissati kisaṃ paṇḍuṃ,",
"ja416:6.4": "sāva sākhā parantapaṃ.",
"ja416:7.1": "Socayissati maṃ kantā,",
"ja416:7.2": "gāme vasamaninditā;",
"ja416:7.3": "Karissati kisaṃ paṇḍuṃ,",
"ja416:7.4": "sāva sākhā parantapaṃ.",
"ja416:8.1": "Tayā maṃ asitāpaṅgi,",
"ja416:8.2": "sitāni bhaṇitāni ca;",
"ja416:8.3": "Kisaṃ paṇḍuṃ karissanti,",
"ja416:8.4": "sāva sākhā parantapaṃ”.",
"ja416:9.1": "“Agamā nūna so saddo,",
"ja416:9.2": "asaṃsi nūna so tava;",
"ja416:9.3": "Akkhātaṃ nūna taṃ tena,",
"ja416:9.4": "yo taṃ sākhamakampayi.",
"ja416:10.1": "Idaṃ kho taṃ samāgamma,",
"ja416:10.2": "mama bālassa cintitaṃ;",
"ja416:10.3": "Tadā hi calitā sākhā,",
"ja416:10.4": "manussena migena vā”.",
"ja416:11.1": "“Tatheva tvaṃ avedesi,",
"ja416:11.2": "Avañci pitaraṃ mama;",
"ja416:11.3": "Hantvā sākhāhi chādento,",
"ja416:11.4": "Āgamissati me bhayan”ti. #1140",
"ja416:12.1": "Parantapajātakaṃ ekādasamaṃ.",
"ja416:12.2": "Gandhāravaggo dutiyo.",
"ja416:13.1": "Tassuddānaṃ",
"ja416:14.1": "Varagāma mahākapi bhaggava ca,",
"ja416:14.2": "Daḷhadhamma sakuñjara kesavaro;",
"ja416:14.3": "Urago vidhuro puna jāgarataṃ,",
"ja416:14.4": "Atha kosalādhipa parantapa cāti.",
"ja416:15.1": "Atha vagguddānaṃ",
"ja416:16.1": "Atha sattanipātamhi,",
"ja416:16.2": "vaggaṃ me bhaṇato suṇa;",
"ja416:16.3": "Kukku ca puna gandhāro,",
"ja416:16.4": "dveva vuttā mahesināti.",
"ja416:17.1": "Sattakanipātaṃ niṭṭhitaṃ."
}