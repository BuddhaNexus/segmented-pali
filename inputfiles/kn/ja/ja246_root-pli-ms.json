{
"ja246:1.1": "Jātaka",
"ja246:2.1": "Dukanipāta",
"ja246:3.1": "Siṅgālavagga",
"ja246:4.1": "6. Bālovādajātaka",
"ja246:5.1": "“Hantvā chetvā vadhitvā ca,",
"ja246:5.2": "deti dānaṃ asaññato;",
"ja246:5.3": "Edisaṃ bhattaṃ bhuñjamāno,",
"ja246:5.4": "sa pāpena upalippati”.",
"ja246:6.1": "“Puttadārampi ce hantvā,",
"ja246:6.2": "Deti dānaṃ asaññato;",
"ja246:6.3": "Bhuñjamānopi sappañño,",
"ja246:6.4": "Na pāpena upalippatī”ti.",
"ja246:7.1": "Bālovādajātakaṃ chaṭṭhaṃ."
}