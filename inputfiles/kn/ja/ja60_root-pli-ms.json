{
"ja60:1.1": "Jātaka",
"ja60:2.1": "Ekakanipāta",
"ja60:3.1": "Āsīsavagga",
"ja60:4.1": "10. Saṅkhadhamajātaka",
"ja60:5.1": "“Dhame dhame nātidhame,",
"ja60:5.2": "atidhantañhi pāpakaṃ;",
"ja60:5.3": "Dhantenādhigatā bhogā,",
"ja60:5.4": "te tāto vidhamī dhaman”ti.",
"ja60:6.1": "Saṅkhadhamajātakaṃ dasamaṃ.",
"ja60:6.2": "Āsīsavaggo chaṭṭho.",
"ja60:7.1": "Tassuddānaṃ",
"ja60:8.1": "Yathā icchiṃ tathāhudakā thalā,",
"ja60:8.2": "Sura sāduphalo ca alīnamano;",
"ja60:8.3": "Sampahaṭṭhamano caturo ca tayo,",
"ja60:8.4": "Sataladdhaka bhogadhanena dasāti."
}