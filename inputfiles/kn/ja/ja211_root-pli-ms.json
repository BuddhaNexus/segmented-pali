{
"ja211:1.1": "Jātaka",
"ja211:2.1": "Dukanipāta",
"ja211:3.1": "Bīraṇathambhavagga",
"ja211:4.1": "1. Somadattajātaka",
"ja211:5.1": "“Akāsi yoggaṃ dhuvamappamatto,",
"ja211:5.2": "Saṃvaccharaṃ bīraṇathambhakasmiṃ;",
"ja211:5.3": "Byākāsi saññaṃ parisaṃ vigayha,",
"ja211:5.4": "Na niyyamo tāyati appapaññaṃ”.",
"ja211:6.1": "“Dvayaṃ yācanako tāta,",
"ja211:6.2": "somadatta nigacchati;",
"ja211:6.3": "Alābhaṃ dhanalābhaṃ vā,",
"ja211:6.4": "evaṃ dhammā hi yācanā”ti.",
"ja211:7.1": "Somadattajātakaṃ paṭhamaṃ."
}