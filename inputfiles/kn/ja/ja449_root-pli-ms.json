{
"ja449:1.1": "Jātaka",
"ja449:2.1": "Dasakanipāta",
"ja449:3.1": "Catudvāravagga",
"ja449:4.1": "11. Maṭṭhakuṇḍalījātaka",
"ja449:5.1": "“Alaṅkato maṭṭhakuṇḍalī,",
"ja449:5.2": "Māladhārī haricandanussado;",
"ja449:5.3": "Bāhā paggayha kandasi,",
"ja449:5.4": "Vanamajjhe kiṃ dukkhito tuvaṃ”.",
"ja449:6.1": "“Sovaṇṇamayo pabhassaro,",
"ja449:6.2": "Uppanno rathapañjaro mama;",
"ja449:6.3": "Tassa cakkayugaṃ na vindāmi,",
"ja449:6.4": "Tena dukkhena jahāmi jīvitaṃ”.",
"ja449:7.1": "“Sovaṇṇamayaṃ maṇīmayaṃ,",
"ja449:7.2": "Lohamayaṃ atha rūpiyāmayaṃ;",
"ja449:7.3": "Pāvada rathaṃ karissāmi te,",
"ja449:7.4": "Cakkayugaṃ paṭipādayāmi taṃ”.",
"ja449:8.1": "“So māṇavo tassa pāvadi,",
"ja449:8.2": "Candasūriyā ubhayettha bhātaro;",
"ja449:8.3": "Sovaṇṇamayo ratho mama,",
"ja449:8.4": "Tena cakkayugena sobhati”.",
"ja449:9.1": "“Bālo kho tvaṃsi māṇava,",
"ja449:9.2": "Yo tvaṃ patthayase apatthiyaṃ;",
"ja449:9.3": "Maññāmi tuvaṃ marissasi,",
"ja449:9.4": "Na hi tvaṃ lacchasi candasūriye”.",
"ja449:10.1": "“Gamanāgamanampi dissati,",
"ja449:10.2": "Vaṇṇadhātu ubhayettha vīthiyo;",
"ja449:10.3": "Peto pana neva dissati,",
"ja449:10.4": "Ko nu kho kandataṃ bālyataro”.",
"ja449:11.1": "“Saccaṃ kho vadesi māṇava,",
"ja449:11.2": "Ahameva kandataṃ bālyataro;",
"ja449:11.3": "Candaṃ viya dārako rudaṃ,",
"ja449:11.4": "Petaṃ kālakatābhipatthaye.",
"ja449:12.1": "Ādittaṃ vata maṃ santaṃ,",
"ja449:12.2": "ghatasittaṃva pāvakaṃ;",
"ja449:12.3": "Vārinā viya osiñcaṃ,",
"ja449:12.4": "sabbaṃ nibbāpaye daraṃ.",
"ja449:13.1": "Abbahī vata me sallaṃ,",
"ja449:13.2": "yamāsi hadayassitaṃ;",
"ja449:13.3": "Yo me sokaparetassa,",
"ja449:13.4": "puttasokaṃ apānudi.",
"ja449:14.1": "Sohaṃ abbūḷhasallosmi,",
"ja449:14.2": "vītasoko anāvilo;",
"ja449:14.3": "Na socāmi na rodāmi,",
"ja449:14.4": "tava sutvāna māṇavā”ti.",
"ja449:15.1": "Maṭṭhakuṇḍalījātakaṃ ekādasamaṃ."
}