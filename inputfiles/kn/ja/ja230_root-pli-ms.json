{
"ja230:1.1": "Jātaka",
"ja230:2.1": "Dukanipāta",
"ja230:3.1": "Kāsāvavagga",
"ja230:4.1": "10. Dutiyapalāyitajātaka",
"ja230:5.1": "“Dhajamaparimitaṃ anantapāraṃ,",
"ja230:5.2": "Duppasahaṃ dhaṅkehi sāgaraṃva;",
"ja230:5.3": "Girimiva anilena duppasayho,",
"ja230:5.4": "Duppasaho ahamajjatādisena”.",
"ja230:6.1": "“Mā bāliyaṃ vilapi na hissa tādisaṃ,",
"ja230:6.2": "Viḍayhase na hi labhase nisedhakaṃ;",
"ja230:6.3": "Āsajjasi gajamiva ekacārinaṃ,",
"ja230:6.4": "Yo taṃ padā naḷamiva pothayissatī”ti.",
"ja230:7.1": "Dutiyapalāyitajātakaṃ dasamaṃ.",
"ja230:7.2": "Kāsāvavaggo aṭṭhamo.",
"ja230:8.1": "Tassuddānaṃ",
"ja230:9.1": "Varavatthavaco dumakhīṇaphalaṃ,",
"ja230:9.2": "Caturodhammavaraṃ purisuttama;",
"ja230:9.3": "Dhaṅkamagadhā ca tayogirināma,",
"ja230:9.4": "Gajaggavaro dhajavarena dasāti."
}