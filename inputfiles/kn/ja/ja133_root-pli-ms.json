{
"ja133:1.1": "Jātaka",
"ja133:2.1": "Ekakanipāta",
"ja133:3.1": "Asampadānavagga",
"ja133:4.1": "3. Ghatāsanajātaka",
"ja133:5.1": "“Khemaṃ yahiṃ tattha arī udīrito,",
"ja133:5.2": "Dakassa majjhe jalate ghatāsano;",
"ja133:5.3": "Na ajja vāso mahiyā mahīruhe,",
"ja133:5.4": "Disā bhajavho saraṇājja no bhayan”ti.",
"ja133:6.1": "Ghatāsanajātakaṃ tatiyaṃ."
}