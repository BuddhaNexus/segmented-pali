{
"ja296:1.1": "Jātaka",
"ja296:2.1": "Tikanipāta",
"ja296:3.1": "Kumbhavagga",
"ja296:4.1": "6. Samuddajātaka",
"ja296:5.1": "“Ko nāyaṃ loṇatoyasmiṃ,",
"ja296:5.2": "samantā paridhāvati;",
"ja296:5.3": "Macche makare ca vāreti,",
"ja296:5.4": "ūmīsu ca vihaññati”.",
"ja296:6.1": "“Anantapāyī sakuṇo,",
"ja296:6.2": "atittoti disāsuto;",
"ja296:6.3": "Samuddaṃ pātumicchāmi,",
"ja296:6.4": "sāgaraṃ saritaṃ patiṃ”.",
"ja296:7.1": "“So ayaṃ hāyati ceva,",
"ja296:7.2": "pūrate ca mahodadhi;",
"ja296:7.3": "Nāssa nāyati pītanto,",
"ja296:7.4": "apeyyo kira sāgaro”ti.",
"ja296:8.1": "Samuddajātakaṃ chaṭṭhaṃ."
}