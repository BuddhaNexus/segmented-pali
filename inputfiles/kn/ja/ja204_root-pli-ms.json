{
  "ja204:0.1": "Jātaka ",
  "ja204:0.2": "Dukanipāta ",
  "ja204:0.3": "Nataṁdaḷhavagga ",
  "ja204:0.4": "4. Vīrakajātaka ",
  "ja204:1.1": "“Api vīraka passesi, ",
  "ja204:1.2": "sakuṇaṁ mañjubhāṇakaṁ; ",
  "ja204:1.3": "Mayūragīvasaṅkāsaṁ, ",
  "ja204:1.4": "patiṁ mayhaṁ saviṭṭhakaṁ”. ",
  "ja204:2.1": "“Udakathalacarassa pakkhino, ",
  "ja204:2.2": "Niccaṁ āmakamacchabhojino; ",
  "ja204:2.3": "Tassānukaraṁ saviṭṭhako, ",
  "ja204:2.4": "Sevāle paliguṇṭhito mato”ti. ",
  "ja204:3.1": "Vīrakajātakaṁ catutthaṁ. "
}