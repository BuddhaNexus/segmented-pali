{
"ja198:1.1": "Jātaka",
"ja198:2.1": "Dukanipāta",
"ja198:3.1": "Ruhakavagga",
"ja198:4.1": "8. Rādhajātaka",
"ja198:5.1": "“Pavāsā āgato tāta,",
"ja198:5.2": "idāni nacirāgato;",
"ja198:5.3": "Kaccinnu tāta te mātā,",
"ja198:5.4": "na aññamupasevati”.",
"ja198:6.1": "“Na kho panetaṃ subhaṇaṃ,",
"ja198:6.2": "Giraṃ saccupasaṃhitaṃ;",
"ja198:6.3": "Sayetha poṭṭhapādova,",
"ja198:6.4": "Mummure upakūthito”ti.",
"ja198:7.1": "Rādhajātakaṃ aṭṭhamaṃ."
}