{
"ja403:1.1": "Jātaka",
"ja403:2.1": "Sattakanipāta",
"ja403:3.1": "Kukkuvagga",
"ja403:4.1": "8. Aṭṭhisenakajātaka",
"ja403:5.1": "“Yeme ahaṃ na jānāmi,",
"ja403:5.2": "aṭṭhisena vanibbake;",
"ja403:5.3": "Te maṃ saṅgamma yācanti,",
"ja403:5.4": "kasmā maṃ tvaṃ na yācasi”.",
"ja403:6.1": "“Yācako appiyo hoti,",
"ja403:6.2": "yācaṃ adadamappiyo;",
"ja403:6.3": "Tasmāhaṃ taṃ na yācāmi,",
"ja403:6.4": "mā me videssanā ahu”.",
"ja403:7.1": "“Yo ve yācanajīvāno,",
"ja403:7.2": "kāle yācaṃ na yācati;",
"ja403:7.3": "Parañca puññā dhaṃseti,",
"ja403:7.4": "attanāpi na jīvati.",
"ja403:8.1": "Yo ca yācanajīvāno,",
"ja403:8.2": "kāle yācañhi yācati;",
"ja403:8.3": "Parañca puññaṃ labbheti,",
"ja403:8.4": "attanāpi ca jīvati.",
"ja403:9.1": "Na ve dessanti sappaññā,",
"ja403:9.2": "Disvā yācakamāgate;",
"ja403:9.3": "Brahmacāri piyo mesi,",
"ja403:9.4": "Vada tvaṃ bhaññamicchasi”.",
"ja403:10.1": "“Na ve yācanti sappaññā,",
"ja403:10.2": "Dhīro ca veditumarahati;",
"ja403:10.3": "Uddissa ariyā tiṭṭhanti,",
"ja403:10.4": "Esā ariyāna yācanā”.",
"ja403:11.1": "“Dadāmi te brāhmaṇa rohiṇīnaṃ,",
"ja403:11.2": "Gavaṃ sahassaṃ saha puṅgavena;",
"ja403:11.3": "Ariyo hi ariyassa kathaṃ na dajjā,",
"ja403:11.4": "Sutvāna gāthā tava dhammayuttā”ti.",
"ja403:12.1": "Aṭṭhisenakajātakaṃ aṭṭhamaṃ."
}