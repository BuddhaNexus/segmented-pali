{
"ja351:1.1": "Jātaka",
"ja351:2.1": "Pañcakanipāta",
"ja351:3.1": "Maṇikuṇḍalavagga",
"ja351:4.1": "1. Maṇikuṇḍalajātaka",
"ja351:5.1": "“Jīno rathassaṃ maṇikuṇḍale ca,",
"ja351:5.2": "Putte ca dāre ca tatheva jīno;",
"ja351:5.3": "Sabbesu bhogesu asesakesu,",
"ja351:5.4": "Kasmā na santappasi sokakāle”.",
"ja351:6.1": "“Pubbeva maccaṃ vijahanti bhogā,",
"ja351:6.2": "Macco vā te pubbataraṃ jahāti;",
"ja351:6.3": "Asassatā bhogino kāmakāmi,",
"ja351:6.4": "Tasmā na socāmahaṃ sokakāle.",
"ja351:7.1": "Udeti āpūrati veti cando,",
"ja351:7.2": "Atthaṃ tapetvāna paleti sūriyo;",
"ja351:7.3": "Viditā mayā sattuka lokadhammā,",
"ja351:7.4": "Tasmā na socāmahaṃ sokakāle”.",
"ja351:8.1": "“Alaso gihī kāmabhogī na sādhu,",
"ja351:8.2": "Asaññato pabbajito na sādhu;",
"ja351:8.3": "Rājā na sādhu anisammakārī,",
"ja351:8.4": "Yo paṇḍito kodhano taṃ na sādhu.",
"ja351:9.1": "Nisamma khattiyo kayirā,",
"ja351:9.2": "nānisamma disampati;",
"ja351:9.3": "Nisammakārino rāja,",
"ja351:9.4": "yaso kitti ca vaḍḍhatī”ti.",
"ja351:10.1": "Maṇikuṇḍalajātakaṃ paṭhamaṃ."
}