{
"ja110:1.1": "Jātaka",
"ja110:2.1": "Ekakanipāta",
"ja110:3.1": "Parosatavagga",
"ja110:4.1": "10. Sabbasaṃhārakapañhajātaka",
"ja110:5.1": "“Sabbasaṃhārako natthi,",
"ja110:5.2": "suddhaṃ kaṅgu pavāyati;",
"ja110:5.3": "Alikaṃ bhāyatiyaṃ dhuttī,",
"ja110:5.4": "saccamāha mahallikā”ti.",
"ja110:6.1": "Sabbasaṃhārakapañhajātakaṃ dasamaṃ.",
"ja110:6.2": "Parosatavaggo ekādasamo.",
"ja110:7.1": "Tassuddānaṃ",
"ja110:8.1": "Saparosatatāyitaverī puna,",
"ja110:8.2": "Bhamacakkatha nāgasirivhayano;",
"ja110:8.3": "Sukhakañca vata sippaka bāhiyā,",
"ja110:8.4": "Kuṇḍapūva mahallikakā ca dasāti."
}