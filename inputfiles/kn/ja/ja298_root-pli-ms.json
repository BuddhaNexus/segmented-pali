{
  "ja298:0.1": "Jātaka ",
  "ja298:0.2": "Tikanipāta ",
  "ja298:0.3": "Kumbhavagga ",
  "ja298:0.4": "8. Udumbarajātaka ",
  "ja298:1.1": "“Udumbarā cime pakkā, ",
  "ja298:1.2": "nigrodhā ca kapitthanā; ",
  "ja298:1.3": "Ehi nikkhama bhuñjassu, ",
  "ja298:1.4": "kiṁ jighacchāya miyyasi”. ",
  "ja298:2.1": "“Evaṁ so suhito hoti, ",
  "ja298:2.2": "yo vuḍḍhamapacāyati; ",
  "ja298:2.3": "Yathāhamajja suhito, ",
  "ja298:2.4": "dumapakkāni māsito”. ",
  "ja298:3.1": "“Yaṁ vanejo vanejassa, ",
  "ja298:3.2": "vañceyya kapino kapi; ",
  "ja298:3.3": "Daharo kapi saddheyya, ",
  "ja298:3.4": "na hi jiṇṇo jarākapī”ti. ",
  "ja298:4.1": "Udumbarajātakaṁ aṭṭhamaṁ. "
}