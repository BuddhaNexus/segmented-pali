{
"ja134:1.1": "Jātaka",
"ja134:2.1": "Ekakanipāta",
"ja134:3.1": "Asampadānavagga",
"ja134:4.1": "4. Jhānasodhanajātaka",
"ja134:5.1": "“Ye saññino tepi duggatā,",
"ja134:5.2": "Yepi asaññino tepi duggatā;",
"ja134:5.3": "Etaṃ ubhayaṃ vivajjaya,",
"ja134:5.4": "Taṃ samāpattisukhaṃ anaṅgaṇan”ti.",
"ja134:6.1": "Jhānasodhanajātakaṃ catutthaṃ."
}