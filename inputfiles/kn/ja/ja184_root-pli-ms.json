{
"ja184:1.1": "Jātaka",
"ja184:2.1": "Dukanipāta",
"ja184:3.1": "Asadisavagga",
"ja184:4.1": "4. Giridattajātaka",
"ja184:5.1": "“Dūsito giridattena,",
"ja184:5.2": "hayo sāmassa paṇḍavo;",
"ja184:5.3": "Porāṇaṃ pakatiṃ hitvā,",
"ja184:5.4": "tassevānuvidhiyyati.",
"ja184:6.1": "Sace ca tanujo poso,",
"ja184:6.2": "sikharākārakappito;",
"ja184:6.3": "Ānane naṃ gahetvāna,",
"ja184:6.4": "maṇḍale parivattaye;",
"ja184:6.5": "Khippameva pahantvāna,",
"ja184:6.6": "tassevānuvidhiyyatī”ti.",
"ja184:7.1": "Giridattajātakaṃ catutthaṃ."
}