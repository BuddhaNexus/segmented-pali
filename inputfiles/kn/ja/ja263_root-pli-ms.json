{
"ja263:1.1": "Jātaka",
"ja263:2.1": "Tikanipāta",
"ja263:3.1": "Padumavagga",
"ja263:4.1": "3. Cūḷapalobhanajātaka",
"ja263:5.1": "“Abhijjamāne vārismiṃ,",
"ja263:5.2": "sayaṃ āgamma iddhiyā;",
"ja263:5.3": "Missībhāvitthiyā gantvā,",
"ja263:5.4": "saṃsīdasi mahaṇṇave.",
"ja263:6.1": "Āvaṭṭanī mahāmāyā,",
"ja263:6.2": "brahmacariyavikopanā;",
"ja263:6.3": "Sīdanti naṃ viditvāna,",
"ja263:6.4": "ārakā parivajjaye.",
"ja263:7.1": "Yaṃ etā upasevanti,",
"ja263:7.2": "chandasā vā dhanena vā;",
"ja263:7.3": "Jātavedova saṃ ṭhānaṃ,",
"ja263:7.4": "khippaṃ anudahanti nan”ti.",
"ja263:8.1": "Cūḷapalobhanajātakaṃ tatiyaṃ."
}