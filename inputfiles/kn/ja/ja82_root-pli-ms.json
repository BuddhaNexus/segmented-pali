{
"ja82:1.1": "Jātaka",
"ja82:2.1": "Ekakanipāta",
"ja82:3.1": "Apāyimhavagga",
"ja82:4.1": "2. Mittavindakajātaka",
"ja82:5.1": "“Atikkamma ramaṇakaṃ,",
"ja82:5.2": "sadāmattañca dūbhakaṃ;",
"ja82:5.3": "Svāsi pāsāṇamāsīno,",
"ja82:5.4": "yasmā jīvaṃ na mokkhasī”ti.",
"ja82:6.1": "Mittavindakajātakaṃ dutiyaṃ."
}