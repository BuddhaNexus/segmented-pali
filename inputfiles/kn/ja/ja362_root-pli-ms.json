{
  "ja362:0.1": "Jātaka ",
  "ja362:0.2": "Pañcakanipāta ",
  "ja362:0.3": "Vaṇṇārohavagga ",
  "ja362:0.4": "2. Sīlavīmaṁsajātaka ",
  "ja362:1.1": "“Sīlaṁ seyyo sutaṁ seyyo, ",
  "ja362:1.2": "iti me saṁsayo ahu; ",
  "ja362:1.3": "Sīlameva sutā seyyo, ",
  "ja362:1.4": "iti me natthi saṁsayo. ",
  "ja362:2.1": "Moghā jāti ca vaṇṇo ca, ",
  "ja362:2.2": "sīlameva kiruttamaṁ; ",
  "ja362:2.3": "Sīlena anupetassa, ",
  "ja362:2.4": "sutenattho na vijjati. ",
  "ja362:3.1": "Khattiyo ca adhammaṭṭho, ",
  "ja362:3.2": "vesso cādhammanissito; ",
  "ja362:3.3": "Te pariccajjubho loke, ",
  "ja362:3.4": "upapajjanti duggatiṁ. ",
  "ja362:4.1": "Khattiyā brāhmaṇā vessā, ",
  "ja362:4.2": "suddā caṇḍālapukkusā; ",
  "ja362:4.3": "Idha dhammaṁ caritvāna, ",
  "ja362:4.4": "bhavanti tidive samā. ",
  "ja362:5.1": "Na vedā samparāyāya, ",
  "ja362:5.2": "Na jāti nāpi bandhavā; ",
  "ja362:5.3": "Sakañca sīlaṁ saṁsuddhaṁ, ",
  "ja362:5.4": "Samparāyāya sukhāya cā”ti. ",
  "ja362:6.1": "Sīlavīmaṁsajātakaṁ dutiyaṁ. "
}