{
"ja433:1.1": "Jātaka",
"ja433:2.1": "Navakanipāta",
"ja433:3.1": "Gijjhavagga",
"ja433:4.1": "7. Lomasakassapajātaka",
"ja433:5.1": "“Assa indasamo rāja,",
"ja433:5.2": "accantaṃ ajarāmaro;",
"ja433:5.3": "Sace tvaṃ yaññaṃ yājeyya,",
"ja433:5.4": "isiṃ lomasakassapaṃ”.",
"ja433:6.1": "“Sasamuddapariyāyaṃ,",
"ja433:6.2": "mahiṃ sāgarakuṇḍalaṃ;",
"ja433:6.3": "Na icche saha nindāya,",
"ja433:6.4": "evaṃ seyya vijānahi.",
"ja433:7.1": "Dhiratthu taṃ yasalābhaṃ,",
"ja433:7.2": "dhanalābhañca brāhmaṇa;",
"ja433:7.3": "Yā vutti vinipātena,",
"ja433:7.4": "adhammacaraṇena vā.",
"ja433:8.1": "Api ce pattamādāya,",
"ja433:8.2": "anagāro paribbaje;",
"ja433:8.3": "Sāyeva jīvikā seyyo,",
"ja433:8.4": "yā cādhammena esanā.",
"ja433:9.1": "Api ce pattamādāya,",
"ja433:9.2": "anagāro paribbaje;",
"ja433:9.3": "Aññaṃ ahiṃsayaṃ loke,",
"ja433:9.4": "api rajjena taṃ varaṃ”.",
"ja433:10.1": "“Balaṃ cando balaṃ suriyo,",
"ja433:10.2": "balaṃ samaṇabrāhmaṇā;",
"ja433:10.3": "Balaṃ velā samuddassa,",
"ja433:10.4": "balātibalamitthiyo.",
"ja433:11.1": "Yathā uggatapaṃ santaṃ,",
"ja433:11.2": "isiṃ lomasakassapaṃ;",
"ja433:11.3": "Pitu atthā candavatī,",
"ja433:11.4": "vājapeyyaṃ ayājayi”.",
"ja433:12.1": "“Taṃ lobhapakataṃ kammaṃ,",
"ja433:12.2": "Kaṭukaṃ kāmahetukaṃ;",
"ja433:12.3": "Tassa mūlaṃ gavesissaṃ,",
"ja433:12.4": "Checchaṃ rāgaṃ sabandhanaṃ.",
"ja433:13.1": "Dhiratthu kāme subahūpi loke,",
"ja433:13.2": "Tapova seyyo kāmaguṇehi rāja;",
"ja433:13.3": "Tapo karissāmi pahāya kāme,",
"ja433:13.4": "Taveva raṭṭhaṃ candavatī ca hotū”ti.",
"ja433:14.1": "Lomasakassapajātakaṃ sattamaṃ."
}