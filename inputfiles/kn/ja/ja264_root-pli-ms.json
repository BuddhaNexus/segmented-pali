{
"ja264:1.1": "Jātaka",
"ja264:2.1": "Tikanipāta",
"ja264:3.1": "Padumavagga",
"ja264:4.1": "4. Mahāpanādajātaka",
"ja264:5.1": "“Panādo nāma so rājā,",
"ja264:5.2": "yassa yūpo suvaṇṇayo;",
"ja264:5.3": "Tiriyaṃ soḷasubbedho,",
"ja264:5.4": "uddhamāhu sahassadhā.",
"ja264:6.1": "Sahassakaṇḍo satageṇḍu,",
"ja264:6.2": "dhajālu haritāmayo;",
"ja264:6.3": "Anaccuṃ tattha gandhabbā,",
"ja264:6.4": "cha sahassāni sattadhā.",
"ja264:7.1": "Evametaṃ tadā āsi,",
"ja264:7.2": "yathā bhāsasi bhaddaji;",
"ja264:7.3": "Sakko ahaṃ tadā āsiṃ,",
"ja264:7.4": "veyyāvaccakaro tavā”ti.",
"ja264:8.1": "Mahāpanādajātakaṃ catutthaṃ."
}