{
"ja344:1.1": "Jātaka",
"ja344:2.1": "Catukkanipāta",
"ja344:3.1": "Cūḷakuṇālavagga",
"ja344:4.1": "4. Ambajātaka",
"ja344:5.1": "“Yo nīliyaṃ maṇḍayati,",
"ja344:5.2": "saṇḍāsena vihaññati;",
"ja344:5.3": "Tassa sā vasamanvetu,",
"ja344:5.4": "yā te ambe avāhari”.",
"ja344:6.1": "“Vīsaṃ vā pañcavīsaṃ vā,",
"ja344:6.2": "ūnatiṃsaṃ va jātiyā;",
"ja344:6.3": "Tādisā pati mā laddhā,",
"ja344:6.4": "yā te ambe avāhari”.",
"ja344:7.1": "“Dīghaṃ gacchatu addhānaṃ,",
"ja344:7.2": "ekikā abhisārikā;",
"ja344:7.3": "Saṅkete pati mā adda,",
"ja344:7.4": "yā te ambe avāhari”.",
"ja344:8.1": "“Alaṅkatā suvasanā,",
"ja344:8.2": "mālinī candanussadā;",
"ja344:8.3": "Ekikā sayane setu,",
"ja344:8.4": "yā te ambe avāharī”ti.",
"ja344:9.1": "Ambajātakaṃ catutthaṃ."
}