{
"ja157:1.1": "Jātaka",
"ja157:2.1": "Dukanipāta",
"ja157:3.1": "Daḷhavagga",
"ja157:4.1": "7. Guṇajātaka",
"ja157:5.1": "“Yena kāmaṃ paṇāmeti,",
"ja157:5.2": "dhammo balavataṃ migī;",
"ja157:5.3": "Unnadantī vijānāhi,",
"ja157:5.4": "jātaṃ saraṇato bhayaṃ”.",
"ja157:6.1": "“Api cepi dubbalo mitto,",
"ja157:6.2": "mittadhammesu tiṭṭhati;",
"ja157:6.3": "So ñātako ca bandhu ca,",
"ja157:6.4": "so mitto so ca me sakhā;",
"ja157:6.5": "Dāṭhini mātimaññittho,",
"ja157:6.6": "siṅgālo mama pāṇado”ti.",
"ja157:7.1": "Guṇajātakaṃ sattamaṃ."
}