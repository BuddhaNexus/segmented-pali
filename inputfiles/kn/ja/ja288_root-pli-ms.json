{
"ja288:1.1": "Jātaka",
"ja288:2.1": "Tikanipāta",
"ja288:3.1": "Abbhantaravagga",
"ja288:4.1": "8. Macchuddānajātaka",
"ja288:5.1": "“Agghanti macchā adhikaṃ sahassaṃ,",
"ja288:5.2": "Na so atthi yo imaṃ saddaheyya;",
"ja288:5.3": "Mayhañca assu idha satta māsā,",
"ja288:5.4": "Ahampi taṃ macchuddānaṃ kiṇeyyaṃ”.",
"ja288:6.1": "“Macchānaṃ bhojanaṃ datvā,",
"ja288:6.2": "mama dakkhiṇamādisi;",
"ja288:6.3": "Taṃ dakkhiṇaṃ sarantiyā,",
"ja288:6.4": "kataṃ apacitiṃ tayā”.",
"ja288:7.1": "“Paduṭṭhacittassa na phāti hoti,",
"ja288:7.2": "Na cāpi taṃ devatā pūjayanti;",
"ja288:7.3": "Yo bhātaraṃ pettikaṃ sāpateyyaṃ,",
"ja288:7.4": "Avañcayī dukkaṭakammakārī”ti.",
"ja288:8.1": "Macchuddānajātakaṃ aṭṭhamaṃ."
}