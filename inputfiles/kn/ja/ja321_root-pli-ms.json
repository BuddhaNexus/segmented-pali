{
"ja321:1.1": "Jātaka",
"ja321:2.1": "Catukkanipāta",
"ja321:3.1": "Kuṭidūsakavagga",
"ja321:4.1": "1. Kuṭidūsakajātaka",
"ja321:5.1": "“Manussasseva te sīsaṃ,",
"ja321:5.2": "hatthapādā ca vānara;",
"ja321:5.3": "Atha kena nu vaṇṇena,",
"ja321:5.4": "agāraṃ te na vijjati”.",
"ja321:6.1": "“Manussasseva me sīsaṃ,",
"ja321:6.2": "hatthapādā ca siṅgila;",
"ja321:6.3": "Yāhu seṭṭhā manussesu,",
"ja321:6.4": "sā me paññā na vijjati”.",
"ja321:7.1": "“Anavaṭṭhitacittassa,",
"ja321:7.2": "lahucittassa dubbhino;",
"ja321:7.3": "Niccaṃ addhuvasīlassa,",
"ja321:7.4": "sukhabhāvo na vijjati.",
"ja321:8.1": "So karassu ānubhāvaṃ,",
"ja321:8.2": "vītivattassu sīliyaṃ;",
"ja321:8.3": "Sītavātaparittāṇaṃ,",
"ja321:8.4": "karassu kuṭavaṃ kapī”ti.",
"ja321:9.1": "Kuṭidūsakajātakaṃ paṭhamaṃ."
}