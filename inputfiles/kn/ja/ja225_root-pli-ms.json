{
"ja225:1.1": "Jātaka",
"ja225:2.1": "Dukanipāta",
"ja225:3.1": "Kāsāvavagga",
"ja225:4.1": "5. Khantivaṇṇajātaka",
"ja225:5.1": "“Atthi me puriso deva,",
"ja225:5.2": "sabbakiccesu byāvaṭo;",
"ja225:5.3": "Tassa cekoparādhatthi,",
"ja225:5.4": "tattha tvaṃ kinti maññasi”.",
"ja225:6.1": "“Amhākampatthi puriso,",
"ja225:6.2": "ediso idha vijjati;",
"ja225:6.3": "Dullabho aṅgasampanno,",
"ja225:6.4": "khantirasmāka ruccatī”ti.",
"ja225:7.1": "Khantivaṇṇajātakaṃ pañcamaṃ."
}