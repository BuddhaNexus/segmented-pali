{
"ja311:1.1": "Jātaka",
"ja311:2.1": "Catukkanipāta",
"ja311:3.1": "Pucimandavagga",
"ja311:4.1": "1. Pucimandajātaka",
"ja311:5.1": "“Uṭṭhehi cora kiṃ sesi,",
"ja311:5.2": "ko attho supanena te;",
"ja311:5.3": "Mā taṃ gahesuṃ rājāno,",
"ja311:5.4": "gāme kibbisakārakaṃ”.",
"ja311:6.1": "“Yannu coraṃ gahessanti,",
"ja311:6.2": "gāme kibbisakārakaṃ;",
"ja311:6.3": "Kiṃ tattha pucimandassa,",
"ja311:6.4": "vane jātassa tiṭṭhato”.",
"ja311:7.1": "“Na tvaṃ assattha jānāsi,",
"ja311:7.2": "mama corassa cantaraṃ;",
"ja311:7.3": "Coraṃ gahetvā rājāno,",
"ja311:7.4": "gāme kibbisakārakaṃ;",
"ja311:7.5": "Appenti nimbasūlasmiṃ,",
"ja311:7.6": "tasmiṃ me saṅkate mano”.",
"ja311:8.1": "“Saṅkeyya saṅkitabbāni,",
"ja311:8.2": "rakkheyyānāgataṃ bhayaṃ;",
"ja311:8.3": "Anāgatabhayā dhīro,",
"ja311:8.4": "ubho loke avekkhatī”ti.",
"ja311:9.1": "Pucimandajātakaṃ paṭhamaṃ."
}