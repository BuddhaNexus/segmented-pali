{
  "ja311:0.1": "Jātaka ",
  "ja311:0.2": "Catukkanipāta ",
  "ja311:0.3": "Pucimandavagga ",
  "ja311:0.4": "1. Pucimandajātaka ",
  "ja311:1.1": "“Uṭṭhehi cora kiṁ sesi, ",
  "ja311:1.2": "ko attho supanena te; ",
  "ja311:1.3": "Mā taṁ gahesuṁ rājāno, ",
  "ja311:1.4": "gāme kibbisakārakaṁ”. ",
  "ja311:2.1": "“Yannu coraṁ gahessanti, ",
  "ja311:2.2": "gāme kibbisakārakaṁ; ",
  "ja311:2.3": "Kiṁ tattha pucimandassa, ",
  "ja311:2.4": "vane jātassa tiṭṭhato”. ",
  "ja311:3.1": "“Na tvaṁ assattha jānāsi, ",
  "ja311:3.2": "mama corassa cantaraṁ; ",
  "ja311:3.3": "Coraṁ gahetvā rājāno, ",
  "ja311:3.4": "gāme kibbisakārakaṁ; ",
  "ja311:3.5": "Appenti nimbasūlasmiṁ, ",
  "ja311:3.6": "tasmiṁ me saṅkate mano”. ",
  "ja311:4.1": "“Saṅkeyya saṅkitabbāni, ",
  "ja311:4.2": "rakkheyyānāgataṁ bhayaṁ; ",
  "ja311:4.3": "Anāgatabhayā dhīro, ",
  "ja311:4.4": "ubho loke avekkhatī”ti. ",
  "ja311:5.1": "Pucimandajātakaṁ paṭhamaṁ. "
}