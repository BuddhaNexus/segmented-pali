{
"ja455:1.1": "Jātaka",
"ja455:2.1": "Ekādasakanipāta",
"ja455:3.1": "Mātuposakavagga",
"ja455:4.1": "1. Mātuposakajātaka",
"ja455:5.1": "“Tassa nāgassa vippavāsena,",
"ja455:5.2": "Virūḷhā sallakī ca kuṭajā ca;",
"ja455:5.3": "Kuruvindakaravīrā bhisasāmā ca,",
"ja455:5.4": "Nivāte pupphitā ca kaṇikārā.",
"ja455:6.1": "Kocideva suvaṇṇakāyurā,",
"ja455:6.2": "Nāgarājaṃ bharanti piṇḍena;",
"ja455:6.3": "Yattha rājā rājakumāro vā,",
"ja455:6.4": "Kavacamabhihessati achambhito”.",
"ja455:7.1": "“Gaṇhāhi nāga kabaḷaṃ,",
"ja455:7.2": "mā nāga kisako bhava;",
"ja455:7.3": "Bahūni rājakiccāni,",
"ja455:7.4": "tāni nāga karissasi”.",
"ja455:8.1": "“Sā nūnasā kapaṇikā,",
"ja455:8.2": "andhā apariṇāyikā;",
"ja455:8.3": "Khāṇuṃ pādena ghaṭṭeti,",
"ja455:8.4": "giriṃ caṇḍoraṇaṃ pati”.",
"ja455:9.1": "“Kā nu te sā mahānāga,",
"ja455:9.2": "andhā apariṇāyikā;",
"ja455:9.3": "Khāṇuṃ pādena ghaṭṭeti,",
"ja455:9.4": "giriṃ caṇḍoraṇaṃ pati”.",
"ja455:10.1": "“Mātā me sā mahārāja,",
"ja455:10.2": "andhā apariṇāyikā;",
"ja455:10.3": "Khāṇuṃ pādena ghaṭṭeti,",
"ja455:10.4": "giriṃ caṇḍoraṇaṃ pati”.",
"ja455:11.1": "“Muñcathetaṃ mahānāgaṃ,",
"ja455:11.2": "yoyaṃ bharati mātaraṃ;",
"ja455:11.3": "Sametu mātarā nāgo,",
"ja455:11.4": "saha sabbehi ñātibhi”.",
"ja455:12.1": "“Mutto ca bandhanā nāgo,",
"ja455:12.2": "muttamādāya kuñjaro;",
"ja455:12.3": "Muhuttaṃ assāsayitvā,",
"ja455:12.4": "agamā yena pabbato.",
"ja455:13.1": "Tato so naḷiniṃ gantvā,",
"ja455:13.2": "sītaṃ kuñjarasevitaṃ;",
"ja455:13.3": "Soṇḍāyūdakamāhatvā,",
"ja455:13.4": "mātaraṃ abhisiñcatha”.",
"ja455:14.1": "“Koyaṃ anariyo devo,",
"ja455:14.2": "akālenapi vassati;",
"ja455:14.3": "Gato me atrajo putto,",
"ja455:14.4": "yo mayhaṃ paricārako”.",
"ja455:15.1": "“Uṭṭhehi amma kiṃ sesi,",
"ja455:15.2": "āgato tyāhamatrajo;",
"ja455:15.3": "Muttomhi kāsirājena,",
"ja455:15.4": "vedehena yasassinā”.",
"ja455:16.1": "“Ciraṃ jīvatu so rājā,",
"ja455:16.2": "kāsīnaṃ raṭṭhavaḍḍhano;",
"ja455:16.3": "Yo me puttaṃ pamocesi,",
"ja455:16.4": "sadā vuddhāpacāyikan”ti.",
"ja455:17.1": "Mātuposakajātakaṃ paṭhamaṃ."
}