{
"ja302:1.1": "Jātaka",
"ja302:2.1": "Catukkanipāta",
"ja302:3.1": "Kāliṅgavagga",
"ja302:4.1": "2. Mahāassārohajātaka",
"ja302:5.1": "“Adeyyesu dadaṃ dānaṃ,",
"ja302:5.2": "deyyesu nappavecchati;",
"ja302:5.3": "Āpāsu byasanaṃ patto,",
"ja302:5.4": "sahāyaṃ nādhigacchati.",
"ja302:6.1": "Nādeyyesu dadaṃ dānaṃ,",
"ja302:6.2": "deyyesu yo pavecchati;",
"ja302:6.3": "Āpāsu byasanaṃ patto,",
"ja302:6.4": "sahāyamadhigacchati.",
"ja302:7.1": "Saññogasambhogavisesadassanaṃ,",
"ja302:7.2": "Anariyadhammesu saṭhesu nassati;",
"ja302:7.3": "Katañca ariyesu ca ajjavesu,",
"ja302:7.4": "Mahapphalaṃ hoti aṇumpi tādisu.",
"ja302:8.1": "Yo pubbe katakalyāṇo,",
"ja302:8.2": "Akā loke sudukkaraṃ;",
"ja302:8.3": "Pacchā kayirā na vā kayirā,",
"ja302:8.4": "Accantaṃ pūjanāraho”ti.",
"ja302:9.1": "Mahāassārohajātakaṃ dutiyaṃ."
}