{
"ja43:1.1": "Jātaka",
"ja43:2.1": "Ekakanipāta",
"ja43:3.1": "Atthakāmavagga",
"ja43:4.1": "3. Veḷukajātaka",
"ja43:5.1": "“Yo atthakāmassa hitānukampino,",
"ja43:5.2": "Ovajjamāno na karoti sāsanaṃ;",
"ja43:5.3": "Evaṃ so nihato seti,",
"ja43:5.4": "Veḷukassa yathā pitā”ti.",
"ja43:6.1": "Veḷukajātakaṃ tatiyaṃ."
}