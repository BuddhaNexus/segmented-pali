{
  "ja255:0.1": "Jātaka ",
  "ja255:0.2": "Tikanipāta ",
  "ja255:0.3": "Saṅkappavagga ",
  "ja255:0.4": "5. Sukajātaka ",
  "ja255:1.1": "“Yāva so mattamaññāsi, ",
  "ja255:1.2": "bhojanasmiṁ vihaṅgamo; ",
  "ja255:1.3": "Tāva addhānamāpādi, ",
  "ja255:1.4": "mātarañca aposayi. ",
  "ja255:2.1": "Yato ca kho bahutaraṁ, ",
  "ja255:2.2": "bhojanaṁ ajjhavāhari; ",
  "ja255:2.3": "Tato tattheva saṁsīdi, ",
  "ja255:2.4": "amattaññū hi so ahu. ",
  "ja255:3.1": "Tasmā mattaññutā sādhu, ",
  "ja255:3.2": "bhojanasmiṁ agiddhatā; ",
  "ja255:3.3": "Amattaññū hi sīdanti, ",
  "ja255:3.4": "mattaññū ca na sīdare”ti. ",
  "ja255:4.1": "Sukajātakaṁ pañcamaṁ. "
}