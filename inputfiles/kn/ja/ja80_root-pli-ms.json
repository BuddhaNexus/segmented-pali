{
"ja80:1.1": "Jātaka",
"ja80:2.1": "Ekakanipāta",
"ja80:3.1": "Varuṇavagga",
"ja80:4.1": "10. Bhīmasenajātaka",
"ja80:5.1": "“Yaṃ te pavikatthitaṃ pure,",
"ja80:5.2": "Atha te pūtisarā sajanti pacchā;",
"ja80:5.3": "Ubhayaṃ na sameti bhīmasena,",
"ja80:5.4": "Yuddhakathā ca idañca te vihaññan”ti.",
"ja80:6.1": "Bhīmasenajātakaṃ dasamaṃ.",
"ja80:6.2": "Varuṇavaggo aṭṭhamo.",
"ja80:7.1": "Tassuddānaṃ",
"ja80:8.1": "Varuṇā akataññūvare tu saccavaraṃ,",
"ja80:8.2": "Savanappatinā ca abhitthanaya;",
"ja80:8.3": "Karuṇāya silāplava illisato,",
"ja80:8.4": "Puna ḍiṇḍimapūtisarena dasāti."
}