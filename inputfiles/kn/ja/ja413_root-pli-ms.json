{
"ja413:1.1": "Jātaka",
"ja413:2.1": "Sattakanipāta",
"ja413:3.1": "Gandhāravagga",
"ja413:4.1": "8. Dhūmakārijātaka",
"ja413:5.1": "“Rājā apucchi vidhuraṃ,",
"ja413:5.2": "dhammakāmo yudhiṭṭhilo;",
"ja413:5.3": "Api brāhmaṇa jānāsi,",
"ja413:5.4": "ko eko bahu socati”.",
"ja413:6.1": "“Brāhmaṇo ajayūthena,",
"ja413:6.2": "pahūtedho vane vasaṃ;",
"ja413:6.3": "Dhūmaṃ akāsi vāseṭṭho,",
"ja413:6.4": "rattindivamatandito.",
"ja413:7.1": "Tassa taṃ dhūmagandhena,",
"ja413:7.2": "sarabhā makasaḍḍitā;",
"ja413:7.3": "Vassāvāsaṃ upāgacchuṃ,",
"ja413:7.4": "dhūmakārissa santike.",
"ja413:8.1": "Sarabhesu manaṃ katvā,",
"ja413:8.2": "ajā so nāvabujjhatha;",
"ja413:8.3": "Āgacchantī vajantī vā,",
"ja413:8.4": "tassa tā vinasuṃ ajā.",
"ja413:9.1": "Sarabhā sarade kāle,",
"ja413:9.2": "pahīnamakase vane;",
"ja413:9.3": "Pāvisuṃ giriduggāni,",
"ja413:9.4": "nadīnaṃ pabhavāni ca.",
"ja413:10.1": "Sarabhe ca gate disvā,",
"ja413:10.2": "ajā ca vibhavaṃ gatā;",
"ja413:10.3": "Kiso ca vivaṇṇo cāsi,",
"ja413:10.4": "paṇḍurogī ca brāhmaṇo.",
"ja413:11.1": "Evaṃ yo saṃ niraṃkatvā,",
"ja413:11.2": "āgantuṃ kurute piyaṃ;",
"ja413:11.3": "So eko bahu socati,",
"ja413:11.4": "dhūmakārīva brāhmaṇo”ti.",
"ja413:12.1": "Dhūmakārijātakaṃ aṭṭhamaṃ."
}