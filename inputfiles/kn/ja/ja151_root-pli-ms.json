{
"ja151:1.1": "Jātaka",
"ja151:2.1": "Dukanipāta",
"ja151:3.1": "Daḷhavagga",
"ja151:4.1": "1. Rājovādajātaka",
"ja151:5.1": "“Daḷhaṃ daḷhassa khipati,",
"ja151:5.2": "balliko mudunā muduṃ;",
"ja151:5.3": "Sādhumpi sādhunā jeti,",
"ja151:5.4": "asādhumpi asādhunā;",
"ja151:5.5": "Etādiso ayaṃ rājā,",
"ja151:5.6": "maggā uyyāhi sārathi”.",
"ja151:6.1": "“Akkodhena jine kodhaṃ,",
"ja151:6.2": "asādhuṃ sādhunā jine;",
"ja151:6.3": "Jine kadariyaṃ dānena,",
"ja151:6.4": "saccenālikavādinaṃ;",
"ja151:6.5": "Etādiso ayaṃ rājā,",
"ja151:6.6": "maggā uyyāhi sārathī”ti.",
"ja151:7.1": "Rājovādajātakaṃ paṭhamaṃ."
}