{
  "ja151:0.1": "Jātaka ",
  "ja151:0.2": "Dukanipāta ",
  "ja151:0.3": "Daḷhavagga ",
  "ja151:0.4": "1. Rājovādajātaka ",
  "ja151:1.1": "“Daḷhaṁ daḷhassa khipati, ",
  "ja151:1.2": "balliko mudunā muduṁ; ",
  "ja151:1.3": "Sādhumpi sādhunā jeti, ",
  "ja151:1.4": "asādhumpi asādhunā; ",
  "ja151:1.5": "Etādiso ayaṁ rājā, ",
  "ja151:1.6": "maggā uyyāhi sārathi”. ",
  "ja151:2.1": "“Akkodhena jine kodhaṁ, ",
  "ja151:2.2": "asādhuṁ sādhunā jine; ",
  "ja151:2.3": "Jine kadariyaṁ dānena, ",
  "ja151:2.4": "saccenālikavādinaṁ; ",
  "ja151:2.5": "Etādiso ayaṁ rājā, ",
  "ja151:2.6": "maggā uyyāhi sārathī”ti. ",
  "ja151:3.1": "Rājovādajātakaṁ paṭhamaṁ. "
}