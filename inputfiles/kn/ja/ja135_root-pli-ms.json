{
"ja135:1.1": "Jātaka",
"ja135:2.1": "Ekakanipāta",
"ja135:3.1": "Asampadānavagga",
"ja135:4.1": "5. Candābhajātaka",
"ja135:5.1": "“Candābhaṃ sūriyābhañca,",
"ja135:5.2": "yodha paññāya gādha”ti.",
"ja135:5.3": "Avitakkena jhānena,",
"ja135:5.4": "hoti ābhassarūpagoti.",
"ja135:6.1": "Candābhajātakaṃ pañcamaṃ."
}