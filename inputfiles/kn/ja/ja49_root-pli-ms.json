{
"ja49:1.1": "Jātaka",
"ja49:2.1": "Ekakanipāta",
"ja49:3.1": "Atthakāmavagga",
"ja49:4.1": "9. Nakkhattajātaka",
"ja49:5.1": "Nakkhattaṃ paṭimānentaṃ,",
"ja49:5.2": "attho bālaṃ upaccagā;",
"ja49:5.3": "Attho atthassa nakkhattaṃ,",
"ja49:5.4": "kiṃ karissanti tārakā”ti.",
"ja49:6.1": "Nakkhattajātakaṃ navamaṃ."
}