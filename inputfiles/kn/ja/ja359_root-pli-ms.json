{
  "ja359:0.1": "Jātaka ",
  "ja359:0.2": "Pañcakanipāta ",
  "ja359:0.3": "Maṇikuṇḍalavagga ",
  "ja359:0.4": "9. Suvaṇṇamigajātaka ",
  "ja359:1.1": "“Vikkama re haripāda, ",
  "ja359:1.2": "vikkama re mahāmiga; ",
  "ja359:1.3": "Chinda vārattikaṁ pāsaṁ, ",
  "ja359:1.4": "nāhaṁ ekā vane rame”. ",
  "ja359:2.1": "“Vikkamāmi na pāremi, ",
  "ja359:2.2": "bhūmiṁ sumbhāmi vegasā; ",
  "ja359:2.3": "Daḷho vārattiko pāso, ",
  "ja359:2.4": "pādaṁ me parikantati”. ",
  "ja359:3.1": "“Attharassu palāsāni, ",
  "ja359:3.2": "asiṁ nibbāha luddaka; ",
  "ja359:3.3": "Paṭhamaṁ maṁ vadhitvāna, ",
  "ja359:3.4": "hana pacchā mahāmigaṁ”. ",
  "ja359:4.1": "“Na me sutaṁ vā diṭṭhaṁ vā, ",
  "ja359:4.2": "Bhāsantiṁ mānusiṁ migiṁ; ",
  "ja359:4.3": "Tvañca bhadde sukhī hohi, ",
  "ja359:4.4": "Eso cāpi mahāmigo”. ",
  "ja359:5.1": "“Evaṁ luddaka nandassu, ",
  "ja359:5.2": "saha sabbehi ñātibhi; ",
  "ja359:5.3": "Yathāhamajja nandāmi, ",
  "ja359:5.4": "muttaṁ disvā mahāmigan”ti. ",
  "ja359:6.1": "Suvaṇṇamigajātakaṁ navamaṁ. "
}