{
"ja359:1.1": "Jātaka",
"ja359:2.1": "Pañcakanipāta",
"ja359:3.1": "Maṇikuṇḍalavagga",
"ja359:4.1": "9. Suvaṇṇamigajātaka",
"ja359:5.1": "“Vikkama re haripāda,",
"ja359:5.2": "vikkama re mahāmiga;",
"ja359:5.3": "Chinda vārattikaṃ pāsaṃ,",
"ja359:5.4": "nāhaṃ ekā vane rame”.",
"ja359:6.1": "“Vikkamāmi na pāremi,",
"ja359:6.2": "bhūmiṃ sumbhāmi vegasā;",
"ja359:6.3": "Daḷho vārattiko pāso,",
"ja359:6.4": "pādaṃ me parikantati”.",
"ja359:7.1": "“Attharassu palāsāni,",
"ja359:7.2": "asiṃ nibbāha luddaka;",
"ja359:7.3": "Paṭhamaṃ maṃ vadhitvāna,",
"ja359:7.4": "hana pacchā mahāmigaṃ”.",
"ja359:8.1": "“Na me sutaṃ vā diṭṭhaṃ vā,",
"ja359:8.2": "Bhāsantiṃ mānusiṃ migiṃ;",
"ja359:8.3": "Tvañca bhadde sukhī hohi,",
"ja359:8.4": "Eso cāpi mahāmigo”.",
"ja359:9.1": "“Evaṃ luddaka nandassu,",
"ja359:9.2": "saha sabbehi ñātibhi;",
"ja359:9.3": "Yathāhamajja nandāmi,",
"ja359:9.4": "muttaṃ disvā mahāmigan”ti.",
"ja359:10.1": "Suvaṇṇamigajātakaṃ navamaṃ."
}