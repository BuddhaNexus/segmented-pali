{
"ja445:1.1": "Jātaka",
"ja445:2.1": "Dasakanipāta",
"ja445:3.1": "Catudvāravagga",
"ja445:4.1": "7. Nigrodhajātaka",
"ja445:5.1": "“Na vāhametaṃ jānāmi,",
"ja445:5.2": "ko vāyaṃ kassa vāti vā;",
"ja445:5.3": "Yathā sākho cari evaṃ,",
"ja445:5.4": "nigrodha kinti maññasi.",
"ja445:6.1": "Tato galavinītena,",
"ja445:6.2": "purisā nīhariṃsu maṃ;",
"ja445:6.3": "Datvā mukhapahārāni,",
"ja445:6.4": "sākhassa vacanaṅkarā.",
"ja445:7.1": "Etādisaṃ dummatinā,",
"ja445:7.2": "akataññuna dubbhinā;",
"ja445:7.3": "Kataṃ anariyaṃ sākhena,",
"ja445:7.4": "sakhinā te janādhipa”.",
"ja445:8.1": "“Na vāhametaṃ jānāmi,",
"ja445:8.2": "napi me koci saṃsati;",
"ja445:8.3": "Yaṃ me tvaṃ samma akkhāsi,",
"ja445:8.4": "sākhena kāraṇaṃ kataṃ.",
"ja445:9.1": "Sakhīnaṃ sājīvakaro,",
"ja445:9.2": "mama sākhassa cūbhayaṃ;",
"ja445:9.3": "Tvaṃ nosissariyaṃ dātā,",
"ja445:9.4": "manussesu mahantataṃ;",
"ja445:9.5": "Tayāmā labbhitā iddhī,",
"ja445:9.6": "ettha me natthi saṃsayo.",
"ja445:10.1": "Yathāpi bījamaggimhi,",
"ja445:10.2": "ḍayhati na virūhati;",
"ja445:10.3": "Evaṃ kataṃ asappurise,",
"ja445:10.4": "nassati na virūhati.",
"ja445:11.1": "Kataññumhi ca posamhi,",
"ja445:11.2": "Sīlavante ariyavuttine;",
"ja445:11.3": "Sukhette viya bījāni,",
"ja445:11.4": "Kataṃ tamhi na nassati”.",
"ja445:12.1": "“Imaṃ jammaṃ nekatikaṃ,",
"ja445:12.2": "asappurisacintakaṃ;",
"ja445:12.3": "Hanantu sākhaṃ sattīhi,",
"ja445:12.4": "nāssa icchāmi jīvitaṃ”.",
"ja445:13.1": "“Khamatassa mahārāja,",
"ja445:13.2": "Pāṇā na paṭiānayā;",
"ja445:13.3": "Khama deva asappurisassa,",
"ja445:13.4": "Nāssa icchāmahaṃ vadhaṃ”.",
"ja445:14.1": "“Nigrodhameva seveyya,",
"ja445:14.2": "na sākhamupasaṃvase;",
"ja445:14.3": "Nigrodhasmiṃ mataṃ seyyo,",
"ja445:14.4": "yañce sākhasmi jīvitan”ti.",
"ja445:15.1": "Nigrodhajātakaṃ sattamaṃ."
}