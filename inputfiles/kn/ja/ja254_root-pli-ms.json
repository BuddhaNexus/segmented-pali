{
  "ja254:0.1": "Jātaka ",
  "ja254:0.2": "Tikanipāta ",
  "ja254:0.3": "Saṅkappavagga ",
  "ja254:0.4": "4. Kuṇḍakakucchisindhavajātaka ",
  "ja254:1.1": "“Bhutvā tiṇaparighāsaṁ, ",
  "ja254:1.2": "bhutvā ācāmakuṇḍakaṁ; ",
  "ja254:1.3": "Etaṁ te bhojanaṁ āsi, ",
  "ja254:1.4": "kasmā dāni na bhuñjasi”. ",
  "ja254:2.1": "“Yattha posaṁ na jānanti, ",
  "ja254:2.2": "jātiyā vinayena vā; ",
  "ja254:2.3": "Bahuṁ tattha mahābrahme, ",
  "ja254:2.4": "api ācāmakuṇḍakaṁ. ",
  "ja254:3.1": "Tvañca kho maṁ pajānāsi, ",
  "ja254:3.2": "yādisāyaṁ hayuttamo; ",
  "ja254:3.3": "Jānanto jānamāgamma, ",
  "ja254:3.4": "na te bhakkhāmi kuṇḍakan”ti. ",
  "ja254:4.1": "Kuṇḍakakucchisindhavajātakaṁ catutthaṁ. "
}