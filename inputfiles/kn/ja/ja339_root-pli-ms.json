{
"ja339:1.1": "Jātaka",
"ja339:2.1": "Catukkanipāta",
"ja339:3.1": "Kokilavagga",
"ja339:4.1": "9. Bāverujātaka",
"ja339:5.1": "“Adassanena morassa,",
"ja339:5.2": "sikhino mañjubhāṇino;",
"ja339:5.3": "Kākaṃ tattha apūjesuṃ,",
"ja339:5.4": "maṃsena ca phalena ca.",
"ja339:6.1": "Yadā ca sarasampanno,",
"ja339:6.2": "moro bāverumāgamā;",
"ja339:6.3": "Atha lābho ca sakkāro,",
"ja339:6.4": "vāyasassa ahāyatha.",
"ja339:7.1": "Yāva nuppajjatī buddho,",
"ja339:7.2": "dhammarājā pabhaṅkaro;",
"ja339:7.3": "Tāva aññe apūjesuṃ,",
"ja339:7.4": "puthū samaṇabrāhmaṇe.",
"ja339:8.1": "Yadā ca sarasampanno,",
"ja339:8.2": "buddho dhammaṃ adesayi;",
"ja339:8.3": "Atha lābho ca sakkāro,",
"ja339:8.4": "titthiyānaṃ ahāyathā”ti.",
"ja339:9.1": "Bāverujātakaṃ navamaṃ."
}