{
  "ja339:0.1": "Jātaka ",
  "ja339:0.2": "Catukkanipāta ",
  "ja339:0.3": "Kokilavagga ",
  "ja339:0.4": "9. Bāverujātaka ",
  "ja339:1.1": "“Adassanena morassa, ",
  "ja339:1.2": "sikhino mañjubhāṇino; ",
  "ja339:1.3": "Kākaṁ tattha apūjesuṁ, ",
  "ja339:1.4": "maṁsena ca phalena ca. ",
  "ja339:2.1": "Yadā ca sarasampanno, ",
  "ja339:2.2": "moro bāverumāgamā; ",
  "ja339:2.3": "Atha lābho ca sakkāro, ",
  "ja339:2.4": "vāyasassa ahāyatha. ",
  "ja339:3.1": "Yāva nuppajjatī buddho, ",
  "ja339:3.2": "dhammarājā pabhaṅkaro; ",
  "ja339:3.3": "Tāva aññe apūjesuṁ, ",
  "ja339:3.4": "puthū samaṇabrāhmaṇe. ",
  "ja339:4.1": "Yadā ca sarasampanno, ",
  "ja339:4.2": "buddho dhammaṁ adesayi; ",
  "ja339:4.3": "Atha lābho ca sakkāro, ",
  "ja339:4.4": "titthiyānaṁ ahāyathā”ti. ",
  "ja339:5.1": "Bāverujātakaṁ navamaṁ. "
}