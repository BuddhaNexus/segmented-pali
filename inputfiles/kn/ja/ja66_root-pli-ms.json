{
"ja66:1.1": "Jātaka",
"ja66:2.1": "Ekakanipāta",
"ja66:3.1": "Itthivagga",
"ja66:4.1": "6. Mudulakkhaṇajātaka",
"ja66:5.1": "“Ekā icchā pure āsi,",
"ja66:5.2": "aladdhā mudulakkhaṇaṃ;",
"ja66:5.3": "Yato laddhā aḷārakkhī,",
"ja66:5.4": "icchā icchaṃ vijāyathā”ti.",
"ja66:6.1": "Mudulakkhaṇajātakaṃ chaṭṭhaṃ."
}