{
"ja469:1.1": "Jātaka",
"ja469:2.1": "Dvādasakanipāta",
"ja469:3.1": "Cūḷakuṇālavagga",
"ja469:4.1": "6. Mahākaṇhajātaka",
"ja469:5.1": "“Kaṇho kaṇho ca ghoro ca,",
"ja469:5.2": "sukkadāṭho pabhāsavā;",
"ja469:5.3": "Baddho pañcahi rajjūhi,",
"ja469:5.4": "kiṃ ravi sunakho tava”.",
"ja469:6.1": "“Nāyaṃ migānamatthāya,",
"ja469:6.2": "usīnaka bhavissati;",
"ja469:6.3": "Manussānaṃ anayo hutvā,",
"ja469:6.4": "tadā kaṇho pamokkhati”.",
"ja469:7.1": "“Pattahatthā samaṇakā,",
"ja469:7.2": "muṇḍā saṅghāṭipārutā;",
"ja469:7.3": "Naṅgalehi kasissanti,",
"ja469:7.4": "tadā kaṇho pamokkhati.",
"ja469:8.1": "Tapassiniyo pabbajitā,",
"ja469:8.2": "muṇḍā saṅghāṭipārutā;",
"ja469:8.3": "Yadā loke gamissanti,",
"ja469:8.4": "tadā kaṇho pamokkhati.",
"ja469:9.1": "Dīghottaroṭṭhā jaṭilā,",
"ja469:9.2": "paṅkadantā rajassirā;",
"ja469:9.3": "Iṇaṃ codāya gacchanti,",
"ja469:9.4": "tadā kaṇho pamokkhati.",
"ja469:10.1": "Adhicca vede sāvittiṃ,",
"ja469:10.2": "yaññatantañca brāhmaṇā;",
"ja469:10.3": "Bhatikāya yajissanti,",
"ja469:10.4": "tadā kaṇho pamokkhati.",
"ja469:11.1": "Mātaraṃ pitarañcāpi,",
"ja469:11.2": "jiṇṇakaṃ gatayobbanaṃ;",
"ja469:11.3": "Pahū santo na bharanti,",
"ja469:11.4": "tadā kaṇho pamokkhati.",
"ja469:12.1": "Mātaraṃ pitarañcāpi,",
"ja469:12.2": "jiṇṇakaṃ gatayobbanaṃ;",
"ja469:12.3": "Bālā tumheti vakkhanti,",
"ja469:12.4": "tadā kaṇho pamokkhati.",
"ja469:13.1": "Ācariyabhariyaṃ sakhiṃ,",
"ja469:13.2": "mātulāniṃ pitucchakiṃ;",
"ja469:13.3": "Yadā loke gamissanti,",
"ja469:13.4": "tadā kaṇho pamokkhati.",
"ja469:14.1": "Asicammaṃ gahetvāna,",
"ja469:14.2": "khaggaṃ paggayha brāhmaṇā;",
"ja469:14.3": "Panthaghātaṃ karissanti,",
"ja469:14.4": "tadā kaṇho pamokkhati.",
"ja469:15.1": "Sukkacchavī vedhaverā,",
"ja469:15.2": "thūlabāhū apātubhā;",
"ja469:15.3": "Mittabhedaṃ karissanti,",
"ja469:15.4": "tadā kaṇho pamokkhati.",
"ja469:16.1": "Māyāvino nekatikā,",
"ja469:16.2": "asappurisacintakā;",
"ja469:16.3": "Yadā loke bhavissanti,",
"ja469:16.4": "tadā kaṇho pamokkhatī”ti.",
"ja469:17.1": "Mahākaṇhajātakaṃ chaṭṭhaṃ."
}