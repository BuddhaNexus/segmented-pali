{
"ja61:1.1": "Jātaka",
"ja61:2.1": "Ekakanipāta",
"ja61:3.1": "Itthivagga",
"ja61:4.1": "1. Asātamantajātaka",
"ja61:5.1": "“Asā lokitthiyo nāma,",
"ja61:5.2": "velā tāsaṃ na vijjati;",
"ja61:5.3": "Sārattā ca pagabbhā ca,",
"ja61:5.4": "sikhī sabbaghaso yathā;",
"ja61:5.5": "Tā hitvā pabbajissāmi,",
"ja61:5.6": "vivekamanubrūhayan”ti.",
"ja61:6.1": "Asātamantajātakaṃ paṭhamaṃ."
}