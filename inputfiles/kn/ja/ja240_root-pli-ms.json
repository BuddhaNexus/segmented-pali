{
"ja240:1.1": "Jātaka",
"ja240:2.1": "Dukanipāta",
"ja240:3.1": "Upāhanavagga",
"ja240:4.1": "10. Mahāpiṅgalajātaka",
"ja240:5.1": "“Sabbo jano hiṃsito piṅgalena,",
"ja240:5.2": "Tasmiṃ mate paccayā vedayanti;",
"ja240:5.3": "Piyo nu te āsi akaṇhanetto,",
"ja240:5.4": "Kasmā nu tvaṃ rodasi dvārapāla”.",
"ja240:6.1": "“Na me piyo āsi akaṇhanetto,",
"ja240:6.2": "Bhāyāmi paccāgamanāya tassa;",
"ja240:6.3": "Ito gato hiṃseyya maccurājaṃ,",
"ja240:6.4": "So hiṃsito āneyya puna idha”.",
"ja240:7.1": "“Daḍḍho vāhasahassehi,",
"ja240:7.2": "sitto ghaṭasatehi so;",
"ja240:7.3": "Parikkhatā ca sā bhūmi,",
"ja240:7.4": "mā bhāyi nāgamissatī”ti.",
"ja240:8.1": "Mahāpiṅgalajātakaṃ dasamaṃ.",
"ja240:8.2": "Upāhanavaggo navamo.",
"ja240:9.1": "Tassuddānaṃ",
"ja240:10.1": "Varupāhana khujja vikaṇṇakako,",
"ja240:10.2": "Asitābhuya pañcamavacchanakho;",
"ja240:10.3": "Dija pemavaruttamaekapadaṃ,",
"ja240:10.4": "Kumināmukha piṅgalakena dasāti."
}