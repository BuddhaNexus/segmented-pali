{
  "ja240:0.1": "Jātaka ",
  "ja240:0.2": "Dukanipāta ",
  "ja240:0.3": "Upāhanavagga ",
  "ja240:0.4": "10. Mahāpiṅgalajātaka ",
  "ja240:1.1": "“Sabbo jano hiṁsito piṅgalena, ",
  "ja240:1.2": "Tasmiṁ mate paccayā vedayanti; ",
  "ja240:1.3": "Piyo nu te āsi akaṇhanetto, ",
  "ja240:1.4": "Kasmā nu tvaṁ rodasi dvārapāla”. ",
  "ja240:2.1": "“Na me piyo āsi akaṇhanetto, ",
  "ja240:2.2": "Bhāyāmi paccāgamanāya tassa; ",
  "ja240:2.3": "Ito gato hiṁseyya maccurājaṁ, ",
  "ja240:2.4": "So hiṁsito āneyya puna idha”. ",
  "ja240:3.1": "“Daḍḍho vāhasahassehi, ",
  "ja240:3.2": "sitto ghaṭasatehi so; ",
  "ja240:3.3": "Parikkhatā ca sā bhūmi, ",
  "ja240:3.4": "mā bhāyi nāgamissatī”ti. ",
  "ja240:4.1": "Mahāpiṅgalajātakaṁ dasamaṁ. ",
  "ja240:4.2": "Upāhanavaggo navamo. ",
  "ja240:5.1": "Tassuddānaṁ ",
  "ja240:6.1": "Varupāhana khujja vikaṇṇakako, ",
  "ja240:6.2": "Asitābhuya pañcamavacchanakho; ",
  "ja240:6.3": "Dija pemavaruttamaekapadaṁ, ",
  "ja240:6.4": "Kumināmukha piṅgalakena dasāti. "
}