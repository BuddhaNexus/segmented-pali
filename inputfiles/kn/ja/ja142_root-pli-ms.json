{
"ja142:1.1": "Jātaka",
"ja142:2.1": "Ekakanipāta",
"ja142:3.1": "Kakaṇṭakavagga",
"ja142:4.1": "2. Siṅgālajātaka",
"ja142:5.1": "“Etañhi te durājānaṃ,",
"ja142:5.2": "yaṃ sesi matasāyikaṃ;",
"ja142:5.3": "Yassa te kaḍḍhamānassa,",
"ja142:5.4": "hatthā daṇḍo na muccatī”ti.",
"ja142:6.1": "Siṅgālajātakaṃ dutiyaṃ."
}