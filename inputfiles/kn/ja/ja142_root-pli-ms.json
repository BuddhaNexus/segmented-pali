{
  "ja142:0.1": "Jātaka ",
  "ja142:0.2": "Ekakanipāta ",
  "ja142:0.3": "Kakaṇṭakavagga ",
  "ja142:0.4": "2. Siṅgālajātaka ",
  "ja142:1.1": "“Etañhi te durājānaṁ, ",
  "ja142:1.2": "yaṁ sesi matasāyikaṁ; ",
  "ja142:1.3": "Yassa te kaḍḍhamānassa, ",
  "ja142:1.4": "hatthā daṇḍo na muccatī”ti. ",
  "ja142:2.1": "Siṅgālajātakaṁ dutiyaṁ. "
}