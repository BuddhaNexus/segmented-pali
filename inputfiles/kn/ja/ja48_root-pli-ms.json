{
  "ja48:0.1": "Jātaka ",
  "ja48:0.2": "Ekakanipāta ",
  "ja48:0.3": "Atthakāmavagga ",
  "ja48:0.4": "8. Vedabbajātaka ",
  "ja48:1.1": "“Anupāyena yo atthaṁ, ",
  "ja48:1.2": "Icchati so vihaññati; ",
  "ja48:1.3": "Cetā haniṁsu vedabbaṁ, ",
  "ja48:1.4": "Sabbe te byasanamajjhagūti. ",
  "ja48:2.1": "Vedabbajātakaṁ aṭṭhamaṁ. "
}