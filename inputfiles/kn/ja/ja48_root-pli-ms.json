{
"ja48:1.1": "Jātaka",
"ja48:2.1": "Ekakanipāta",
"ja48:3.1": "Atthakāmavagga",
"ja48:4.1": "8. Vedabbajātaka",
"ja48:5.1": "“Anupāyena yo atthaṃ,",
"ja48:5.2": "Icchati so vihaññati;",
"ja48:5.3": "Cetā haniṃsu vedabbaṃ,",
"ja48:5.4": "Sabbe te byasanamajjhagūti.",
"ja48:6.1": "Vedabbajātakaṃ aṭṭhamaṃ."
}