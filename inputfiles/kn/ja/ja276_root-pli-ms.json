{
"ja276:1.1": "Jātaka",
"ja276:2.1": "Tikanipāta",
"ja276:3.1": "Udapānavagga",
"ja276:4.1": "6. Garudhammajātaka",
"ja276:5.1": "“Tava saddhañca sīlañca,",
"ja276:5.2": "viditvāna janādhipa;",
"ja276:5.3": "Vaṇṇaṃ añjanavaṇṇena,",
"ja276:5.4": "kāliṅgasmiṃ nimimhase”.",
"ja276:6.1": "“Annabhaccā cabhaccā ca,",
"ja276:6.2": "yodha uddissa gacchati;",
"ja276:6.3": "Sabbe te appaṭikkhippā,",
"ja276:6.4": "pubbācariyavaco idaṃ.",
"ja276:7.1": "Dadāmi vo brāhmaṇā nāgametaṃ,",
"ja276:7.2": "Rājārahaṃ rājabhoggaṃ yasassinaṃ;",
"ja276:7.3": "Alaṅkataṃ hemajālābhichannaṃ,",
"ja276:7.4": "Sasārathiṃ gacchatha yena kāman”ti.",
"ja276:8.1": "Garudhammajātakaṃ chaṭṭhaṃ."
}