{
  "ja319:0.1": "Jātaka ",
  "ja319:0.2": "Catukkanipāta ",
  "ja319:0.3": "Pucimandavagga ",
  "ja319:0.4": "9. Tittirajātaka ",
  "ja319:1.1": "“Susukhaṁ vata jīvāmi, ",
  "ja319:1.2": "labhāmi ceva bhuñjituṁ; ",
  "ja319:1.3": "Paripanthe ca tiṭṭhāmi, ",
  "ja319:1.4": "kā nu bhante gatī mama”. ",
  "ja319:2.1": "“Mano ce te nappaṇamati, ",
  "ja319:2.2": "pakkhi pāpassa kammuno; ",
  "ja319:2.3": "Abyāvaṭassa bhadrassa, ",
  "ja319:2.4": "na pāpamupalimpati”. ",
  "ja319:3.1": "“Ñātako no nisinnoti, ",
  "ja319:3.2": "bahu āgacchate jano; ",
  "ja319:3.3": "Paṭicca kammaṁ phusati, ",
  "ja319:3.4": "tasmiṁ me saṅkate mano”. ",
  "ja319:4.1": "“Na paṭicca kammaṁ phusati, ",
  "ja319:4.2": "mano ce nappadussati; ",
  "ja319:4.3": "Appossukkassa bhadrassa, ",
  "ja319:4.4": "na pāpamupalimpatī”ti. ",
  "ja319:5.1": "Tittirajātakaṁ navamaṁ. "
}