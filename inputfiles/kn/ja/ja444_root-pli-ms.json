{
  "ja444:0.1": "Jātaka ",
  "ja444:0.2": "Dasakanipāta ",
  "ja444:0.3": "Catudvāravagga ",
  "ja444:0.4": "6. Kaṇhadīpāyanajātaka ",
  "ja444:1.1": "“Sattāhamevāhaṁ pasannacitto, ",
  "ja444:1.2": "Puññatthiko ācariṁ brahmacariyaṁ; ",
  "ja444:1.3": "Athāparaṁ yaṁ caritaṁ mamedaṁ, ",
  "ja444:1.4": "Vassāni paññāsa samādhikāni; ",
  "ja444:1.5": "Akāmakovāpi ahaṁ carāmi, ",
  "ja444:1.6": "Etena saccena suvatthi hotu; ",
  "ja444:1.7": "Hataṁ visaṁ jīvatu yaññadatto”. ",
  "ja444:2.1": "“Yasmā dānaṁ nābhinandiṁ kadāci, ",
  "ja444:2.2": "Disvānahaṁ atithiṁ vāsakāle; ",
  "ja444:2.3": "Na cāpi me appiyataṁ aveduṁ, ",
  "ja444:2.4": "Bahussutā samaṇabrāhmaṇā ca”. ",
  "ja444:2.5": "Akāmakovāpi ahaṁ dadāmi, ",
  "ja444:2.6": "Etena saccena suvatthi hotu; ",
  "ja444:2.7": "Hataṁ visaṁ jīvatu yaññadatto. ",
  "ja444:3.1": "“Āsīviso tāta pahūtatejo, ",
  "ja444:3.2": "Yo taṁ adaṁsī sacarā udicca; ",
  "ja444:3.3": "Tasmiñca me appiyatāya ajja, ",
  "ja444:3.4": "Pitari ca te natthi koci viseso; ",
  "ja444:3.5": "Etena saccena suvatthi hotu, ",
  "ja444:3.6": "Hataṁ visaṁ jīvatu yaññadatto”. ",
  "ja444:4.1": "“Santā dantāyeva paribbajanti, ",
  "ja444:4.2": "Aññatra kaṇhā natthākāmarūpā; ",
  "ja444:4.3": "Dīpāyana kissa jigucchamāno, ",
  "ja444:4.4": "Akāmako carasi brahmacariyaṁ”. ",
  "ja444:5.1": "“Saddhāya nikkhamma punaṁ nivatto, ",
  "ja444:5.2": "So eḷamūgova bālo vatāyaṁ; ",
  "ja444:5.3": "Etassa vādassa jigucchamāno, ",
  "ja444:5.4": "Akāmako carāmi brahmacariyaṁ; ",
  "ja444:5.5": "Viññuppasatthañca satañca ṭhānaṁ, ",
  "ja444:5.6": "Evampahaṁ puññakaro bhavāmi”. ",
  "ja444:6.1": "“Samaṇe tuvaṁ brāhmaṇe addhike ca, ",
  "ja444:6.2": "Santappayāsi annapānena bhikkhaṁ; ",
  "ja444:6.3": "Opānabhūtaṁva gharaṁ tava yidaṁ, ",
  "ja444:6.4": "Annena pānena upetarūpaṁ; ",
  "ja444:6.5": "Atha kissa vādassa jigucchamāno, ",
  "ja444:6.6": "Akāmako dānamimaṁ dadāsi”. ",
  "ja444:7.1": "“Pitaro ca me āsuṁ pitāmahā ca, ",
  "ja444:7.2": "Saddhā ahuṁ dānapatī vadaññū; ",
  "ja444:7.3": "Taṁ kullavattaṁ anuvattamāno, ",
  "ja444:7.4": "Māhaṁ kule antimagandhano ahuṁ; ",
  "ja444:7.5": "Etassa vādassa jigucchamāno, ",
  "ja444:7.6": "Akāmako dānamimaṁ dadāmi”. ",
  "ja444:8.1": "“Dahariṁ kumāriṁ asamatthapaññaṁ, ",
  "ja444:8.2": "Yaṁ tānayiṁ ñātikulā sugatte; ",
  "ja444:8.3": "Na cāpi me appiyataṁ avedi, ",
  "ja444:8.4": "Aññatra kāmā paricārayantā; ",
  "ja444:8.5": "Atha kena vaṇṇena mayā te bhoti, ",
  "ja444:8.6": "Saṁvāsadhammo ahu evarūpo”. ",
  "ja444:9.1": "“Ārā dūre nayidha kadāci atthi, ",
  "ja444:9.2": "Paramparā nāma kule imasmiṁ; ",
  "ja444:9.3": "Taṁ kullavattaṁ anuvattamānā, ",
  "ja444:9.4": "Māhaṁ kule antimagandhinī ahuṁ; ",
  "ja444:9.5": "Etassa vādassa jigucchamānā, ",
  "ja444:9.6": "Akāmikā paddhacarāmhi tuyhaṁ”. ",
  "ja444:10.1": "“Maṇḍabya bhāsiṁ yamabhāsaneyyaṁ, ",
  "ja444:10.2": "Taṁ khamyataṁ puttakahetu majja; ",
  "ja444:10.3": "Puttapemā na idha paratthi kiñci, ",
  "ja444:10.4": "So no ayaṁ jīvati yaññadatto”ti. ",
  "ja444:11.1": "Kaṇhadīpāyanajātakaṁ chaṭṭhaṁ. "
}