{
"ja148:1.1": "Jātaka",
"ja148:2.1": "Ekakanipāta",
"ja148:3.1": "Kakaṇṭakavagga",
"ja148:4.1": "8. Siṅgālajātaka",
"ja148:5.1": "“Nāhaṃ punaṃ na ca punaṃ,",
"ja148:5.2": "na cāpi apunappunaṃ;",
"ja148:5.3": "Hatthibondiṃ pavekkhāmi,",
"ja148:5.4": "tathā hi bhayatajjito”ti.",
"ja148:6.1": "Siṅgālajātakaṃ aṭṭhamaṃ."
}