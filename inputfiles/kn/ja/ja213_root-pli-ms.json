{
  "ja213:0.1": "Jātaka ",
  "ja213:0.2": "Dukanipāta ",
  "ja213:0.3": "Bīraṇathambhavagga ",
  "ja213:0.4": "3. Bharujātaka ",
  "ja213:1.1": "“Isīnamantaraṁ katvā, ",
  "ja213:1.2": "bharurājāti me sutaṁ; ",
  "ja213:1.3": "Ucchinno saha raṭṭhehi, ",
  "ja213:1.4": "sarājā vibhavaṅgato. ",
  "ja213:2.1": "Tasmā hi chandāgamanaṁ, ",
  "ja213:2.2": "nappasaṁsanti paṇḍitā; ",
  "ja213:2.3": "Aduṭṭhacitto bhāseyya, ",
  "ja213:2.4": "giraṁ saccupasaṁhitan”ti. ",
  "ja213:3.1": "Bharujātakaṁ tatiyaṁ. "
}