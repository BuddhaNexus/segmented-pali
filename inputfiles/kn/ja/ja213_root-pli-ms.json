{
"ja213:1.1": "Jātaka",
"ja213:2.1": "Dukanipāta",
"ja213:3.1": "Bīraṇathambhavagga",
"ja213:4.1": "3. Bharujātaka",
"ja213:5.1": "“Isīnamantaraṃ katvā,",
"ja213:5.2": "bharurājāti me sutaṃ;",
"ja213:5.3": "Ucchinno saha raṭṭhehi,",
"ja213:5.4": "sarājā vibhavaṅgato.",
"ja213:6.1": "Tasmā hi chandāgamanaṃ,",
"ja213:6.2": "nappasaṃsanti paṇḍitā;",
"ja213:6.3": "Aduṭṭhacitto bhāseyya,",
"ja213:6.4": "giraṃ saccupasaṃhitan”ti.",
"ja213:7.1": "Bharujātakaṃ tatiyaṃ."
}