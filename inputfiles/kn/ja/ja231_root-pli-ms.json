{
"ja231:1.1": "Jātaka",
"ja231:2.1": "Dukanipāta",
"ja231:3.1": "Upāhanavagga",
"ja231:4.1": "1. Upāhanajātaka",
"ja231:5.1": "“Yathāpi kītā purisassupāhanā,",
"ja231:5.2": "Sukhassa atthāya dukhaṃ udabbahe;",
"ja231:5.3": "Ghammābhitattā thalasā papīḷitā,",
"ja231:5.4": "Tasseva pāde purisassa khādare.",
"ja231:6.1": "Evameva yo dukkulīno anariyo,",
"ja231:6.2": "Tammāka vijjañca sutañca ādiya;",
"ja231:6.3": "Tameva so tattha sutena khādati,",
"ja231:6.4": "Anariyo vuccati dupāhanūpamo”ti.",
"ja231:7.1": "Upāhanajātakaṃ paṭhamaṃ."
}