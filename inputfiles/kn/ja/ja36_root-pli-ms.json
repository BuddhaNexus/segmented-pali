{
"ja36:1.1": "Jātaka",
"ja36:2.1": "Ekakanipāta",
"ja36:3.1": "Kulāvakavagga",
"ja36:4.1": "6. Sakuṇajātaka",
"ja36:5.1": "“Yaṃ nissitā jagatiruhaṃ vihaṅgamā,",
"ja36:5.2": "Svāyaṃ aggiṃ pamuñcati;",
"ja36:5.3": "Disā bhajatha vakkaṅgā,",
"ja36:5.4": "Jātaṃ saraṇato bhayan”ti.",
"ja36:6.1": "Sakuṇajātakaṃ chaṭṭhaṃ."
}