{
"ja366:1.1": "Jātaka",
"ja366:2.1": "Pañcakanipāta",
"ja366:3.1": "Vaṇṇārohavagga",
"ja366:4.1": "6. Gumbiyajātaka",
"ja366:5.1": "“Madhuvaṇṇaṃ madhurasaṃ,",
"ja366:5.2": "madhugandhaṃ visaṃ ahu;",
"ja366:5.3": "Gumbiyo ghāsamesāno,",
"ja366:5.4": "araññe odahī visaṃ.",
"ja366:6.1": "Madhu iti maññamānā,",
"ja366:6.2": "ye taṃ visamakhādisuṃ;",
"ja366:6.3": "Tesaṃ taṃ kaṭukaṃ āsi,",
"ja366:6.4": "maraṇaṃ tenupāgamuṃ.",
"ja366:7.1": "Ye ca kho paṭisaṅkhāya,",
"ja366:7.2": "visaṃ taṃ parivajjayuṃ;",
"ja366:7.3": "Te āturesu sukhitā,",
"ja366:7.4": "ḍayhamānesu nibbutā.",
"ja366:8.1": "Evameva manussesu,",
"ja366:8.2": "visaṃ kāmā samohitā;",
"ja366:8.3": "Āmisaṃ bandhanañcetaṃ,",
"ja366:8.4": "maccuveso guhāsayo.",
"ja366:9.1": "Evameva ime kāme,",
"ja366:9.2": "āturā paricārike;",
"ja366:9.3": "Ye sadā parivajjenti,",
"ja366:9.4": "saṅgaṃ loke upaccagun”ti.",
"ja366:10.1": "Gumbiyajātakaṃ chaṭṭhaṃ."
}