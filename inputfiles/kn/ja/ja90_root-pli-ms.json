{
"ja90:1.1": "Jātaka",
"ja90:2.1": "Ekakanipāta",
"ja90:3.1": "Apāyimhavagga",
"ja90:4.1": "10. Akataññujātaka",
"ja90:5.1": "“Yo pubbe katakalyāṇo,",
"ja90:5.2": "katattho nāvabujjhati;",
"ja90:5.3": "Pacchā kicce samuppanne,",
"ja90:5.4": "kattāraṃ nādhigacchatī”ti.",
"ja90:6.1": "Akataññujātakaṃ dasamaṃ.",
"ja90:6.2": "Apāyimhavaggo navamo.",
"ja90:7.1": "Tassuddānaṃ",
"ja90:8.1": "Apāyimha ca dūbhaka sattapadaṃ,",
"ja90:8.2": "Chaḷadvara ca āyatinā ca puna;",
"ja90:8.3": "Ahisīlava maṅgali pāpikassā,",
"ja90:8.4": "Sataṃnikkha katatthavarena dasāti."
}