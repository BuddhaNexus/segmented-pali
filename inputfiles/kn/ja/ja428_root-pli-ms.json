{
"ja428:1.1": "Jātaka",
"ja428:2.1": "Navakanipāta",
"ja428:3.1": "Gijjhavagga",
"ja428:4.1": "2. Kosambiyajātaka",
"ja428:5.1": "“Puthusaddo samajano,",
"ja428:5.2": "na bālo koci maññatha;",
"ja428:5.3": "Saṃghasmiṃ bhijjamānasmiṃ,",
"ja428:5.4": "nāññaṃ bhiyyo amaññaruṃ.",
"ja428:6.1": "Parimuṭṭhā paṇḍitābhāsā,",
"ja428:6.2": "vācāgocarabhāṇino;",
"ja428:6.3": "Yāvicchanti mukhāyāmaṃ,",
"ja428:6.4": "yena nītā na taṃ vidū.",
"ja428:7.1": "Akkocchi maṃ avadhi maṃ,",
"ja428:7.2": "ajini maṃ ahāsi me;",
"ja428:7.3": "Ye ca taṃ upanayhanti,",
"ja428:7.4": "veraṃ tesaṃ na sammati.",
"ja428:8.1": "Akkocchi maṃ avadhi maṃ,",
"ja428:8.2": "ajini maṃ ahāsi me;",
"ja428:8.3": "Ye ca taṃ nupanayhanti,",
"ja428:8.4": "veraṃ tesūpasammati.",
"ja428:9.1": "Na hi verena verāni,",
"ja428:9.2": "sammantīdha kudācanaṃ;",
"ja428:9.3": "Averena ca sammanti,",
"ja428:9.4": "esa dhammo sanantano.",
"ja428:10.1": "Pare ca na vijānanti,",
"ja428:10.2": "mayamettha yamāmase;",
"ja428:10.3": "Ye ca tattha vijānanti,",
"ja428:10.4": "tato sammanti medhagā.",
"ja428:11.1": "Aṭṭhicchinnā pāṇaharā,",
"ja428:11.2": "gavāssa dhanahārino;",
"ja428:11.3": "Raṭṭhaṃ vilumpamānānaṃ,",
"ja428:11.4": "tesampi hoti saṅgati;",
"ja428:11.5": "Kasmā tumhāka no siyā.",
"ja428:12.1": "Sace labhetha nipakaṃ sahāyaṃ,",
"ja428:12.2": "Saddhiṃ caraṃ sādhuvihāridhīraṃ;",
"ja428:12.3": "Abhibhuyya sabbāni parissayāni,",
"ja428:12.4": "Careyya tenattamano satīmā.",
"ja428:13.1": "No ce labhetha nipakaṃ sahāyaṃ,",
"ja428:13.2": "Saddhiṃ caraṃ sādhuvihāridhīraṃ;",
"ja428:13.3": "Rājāva raṭṭhaṃ vijitaṃ pahāya,",
"ja428:13.4": "Eko care mātaṅgaraññeva nāgo.",
"ja428:14.1": "Ekassa caritaṃ seyyo,",
"ja428:14.2": "Natthi bāle sahāyatā;",
"ja428:14.3": "Eko care na pāpāni kayirā,",
"ja428:14.4": "Appossukko mātaṅgaraññeva nāgo”ti.",
"ja428:15.1": "Kosambiyajātakaṃ dutiyaṃ."
}