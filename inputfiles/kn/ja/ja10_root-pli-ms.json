{
  "ja10:0.1": "Jātaka ",
  "ja10:0.2": "Ekakanipāta ",
  "ja10:0.3": "Apaṇṇakavagga ",
  "ja10:0.4": "10. Sukhavihārijātaka ",
  "ja10:1.1": "“Yañca aññe na rakkhanti, ",
  "ja10:1.2": "yo ca aññe na rakkhati; ",
  "ja10:1.3": "Sa ve rāja sukhaṁ seti, ",
  "ja10:1.4": "kāmesu anapekkhavā”ti. ",
  "ja10:2.1": "Sukhavihārijātakaṁ dasamaṁ. ",
  "ja10:2.2": "Apaṇṇakavaggo paṭhamo. ",
  "ja10:3.1": "Tassuddānaṁ ",
  "ja10:4.1": "Varāpaṇṇaka vaṇṇupatha serivaro, ",
  "ja10:4.2": "Suvicakkhaṇa taṇḍulanāḷikassā; ",
  "ja10:4.3": "Hiri puttavaruttagāmaṇinā, ",
  "ja10:4.4": "Yo ca na rakkhati tena dasāti. "
}