{
  "ja376:0.1": "Jātaka ",
  "ja376:0.2": "Chakkanipāta ",
  "ja376:0.3": "Avāriyavagga ",
  "ja376:0.4": "1. Avāriyajātaka ",
  "ja376:1.1": "“Māsu kujjha bhūmipati, ",
  "ja376:1.2": "māsu kujjha rathesabha; ",
  "ja376:1.3": "Kuddhaṁ appaṭikujjhanto, ",
  "ja376:1.4": "rājā raṭṭhassa pūjito. ",
  "ja376:2.1": "Gāme vā yadi vāraññe, ",
  "ja376:2.2": "ninne vā yadi vā thale; ",
  "ja376:2.3": "Sabbattha anusāsāmi, ",
  "ja376:2.4": "māsu kujjha rathesabha”. ",
  "ja376:3.1": "“Avāriyapitā nāma, ",
  "ja376:3.2": "ahu gaṅgāya nāviko; ",
  "ja376:3.3": "Pubbe janaṁ tāretvāna, ",
  "ja376:3.4": "pacchā yācati vetanaṁ; ",
  "ja376:3.5": "Tenassa bhaṇḍanaṁ hoti, ",
  "ja376:3.6": "na ca bhogehi vaḍḍhati”. ",
  "ja376:4.1": "“Atiṇṇaṁyeva yācassu, ",
  "ja376:4.2": "apāraṁ tāta nāvika; ",
  "ja376:4.3": "Añño hi tiṇṇassa mano, ",
  "ja376:4.4": "añño hoti pāresino. ",
  "ja376:5.1": "Gāme vā yadi vāraññe, ",
  "ja376:5.2": "ninne vā yadi vā thale; ",
  "ja376:5.3": "Sabbattha anusāsāmi, ",
  "ja376:5.4": "māsu kujjhittha nāvika”. ",
  "ja376:6.1": "“Yāyevānusāsaniyā, ",
  "ja376:6.2": "rājā gāmavaraṁ adā; ",
  "ja376:6.3": "Tāyevānusāsaniyā, ",
  "ja376:6.4": "nāviko paharī mukhaṁ. ",
  "ja376:7.1": "Bhattaṁ bhinnaṁ hatā bhariyā, ",
  "ja376:7.2": "Gabbho ca patito chamā; ",
  "ja376:7.3": "Migova jātarūpena, ",
  "ja376:7.4": "Na tenatthaṁ abandhi sū”ti. ",
  "ja376:8.1": "Avāriyajātakaṁ paṭhamaṁ. "
}