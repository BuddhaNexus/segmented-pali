{
"ja383:1.1": "Jātaka",
"ja383:2.1": "Chakkanipāta",
"ja383:3.1": "Avāriyavagga",
"ja383:4.1": "8. Kukkuṭajātaka",
"ja383:5.1": "“Sucittapattachadana,",
"ja383:5.2": "tambacūḷa vihaṅgama;",
"ja383:5.3": "Oroha dumasākhāya,",
"ja383:5.4": "mudhā bhariyā bhavāmi te”.",
"ja383:6.1": "“Catuppadī tvaṃ kalyāṇi,",
"ja383:6.2": "dvipadāhaṃ manorame;",
"ja383:6.3": "Migī pakkhī asaññuttā,",
"ja383:6.4": "aññaṃ pariyesa sāmikaṃ”.",
"ja383:7.1": "“Komārikā te hessāmi,",
"ja383:7.2": "Mañjukā piyabhāṇinī;",
"ja383:7.3": "Vinda maṃ ariyena vedena,",
"ja383:7.4": "Sāvaya maṃ yadicchasi”.",
"ja383:8.1": "“Kuṇapādini lohitape,",
"ja383:8.2": "cori kukkuṭapothini;",
"ja383:8.3": "Na tvaṃ ariyena vedena,",
"ja383:8.4": "mamaṃ bhattāramicchasi”.",
"ja383:9.1": "“Evampi caturā nārī,",
"ja383:9.2": "disvāna sadhanaṃ naraṃ;",
"ja383:9.3": "Nenti saṇhāhi vācāhi,",
"ja383:9.4": "biḷārī viya kukkuṭaṃ.",
"ja383:10.1": "Yo ca uppatitaṃ atthaṃ,",
"ja383:10.2": "na khippamanubujjhati;",
"ja383:10.3": "Amittavasamanveti,",
"ja383:10.4": "pacchā ca anutappati.",
"ja383:11.1": "Yo ca uppatitaṃ atthaṃ,",
"ja383:11.2": "khippameva nibodhati;",
"ja383:11.3": "Muccate sattusambādhā,",
"ja383:11.4": "kukkuṭova biḷāriyā”ti.",
"ja383:12.1": "Kukkuṭajātakaṃ aṭṭhamaṃ."
}