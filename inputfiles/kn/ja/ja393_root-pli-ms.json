{
  "ja393:0.1": "Jātaka ",
  "ja393:0.2": "Chakkanipāta ",
  "ja393:0.3": "Kharaputtavagga ",
  "ja393:0.4": "8. Vighāsādajātaka ",
  "ja393:1.1": "“Susukhaṁ vata jīvanti, ",
  "ja393:1.2": "ye janā vighāsādino; ",
  "ja393:1.3": "Diṭṭheva dhamme pāsaṁsā, ",
  "ja393:1.4": "samparāye ca suggatī”. ",
  "ja393:2.1": "“Sukassa bhāsamānassa, ",
  "ja393:2.2": "na nisāmetha paṇḍitā; ",
  "ja393:2.3": "Idaṁ suṇātha sodariyā, ",
  "ja393:2.4": "amhevāyaṁ pasaṁsati”. ",
  "ja393:3.1": "“Nāhaṁ tumhe pasaṁsāmi, ",
  "ja393:3.2": "kuṇapādā suṇātha me; ",
  "ja393:3.3": "Ucchiṭṭhabhojino tumhe, ",
  "ja393:3.4": "na tumhe vighāsādino”. ",
  "ja393:4.1": "“Sattavassā pabbajitā, ",
  "ja393:4.2": "mejjhāraññe sikhaṇḍino; ",
  "ja393:4.3": "Vighāseneva yāpentā, ",
  "ja393:4.4": "mayañce bhoto gārayhā; ",
  "ja393:4.5": "Ke nu bhoto pasaṁsiyā”. ",
  "ja393:5.1": "“Tumhe sīhānaṁ byagghānaṁ, ",
  "ja393:5.2": "vāḷānañcāvasiṭṭhakaṁ; ",
  "ja393:5.3": "Ucchiṭṭheneva yāpentā, ",
  "ja393:5.4": "maññivho vighāsādino. ",
  "ja393:6.1": "Ye brāhmaṇassa samaṇassa, ",
  "ja393:6.2": "aññassa vā vanibbino; ",
  "ja393:6.3": "Datvāva sesaṁ bhuñjanti, ",
  "ja393:6.4": "te janā vighāsādino”ti. ",
  "ja393:7.1": "Vighāsādajātakaṁ aṭṭhamaṁ. "
}