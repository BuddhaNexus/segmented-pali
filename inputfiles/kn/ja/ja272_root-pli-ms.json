{
"ja272:1.1": "Jātaka",
"ja272:2.1": "Tikanipāta",
"ja272:3.1": "Udapānavagga",
"ja272:4.1": "2. Byagghajātaka",
"ja272:5.1": "“Yena mittena saṃsaggā,",
"ja272:5.2": "yogakkhemo vihiyyati;",
"ja272:5.3": "Pubbevajjhābhavaṃ tassa,",
"ja272:5.4": "rukkhe akkhīva paṇḍito.",
"ja272:6.1": "Yena mittena saṃsaggā,",
"ja272:6.2": "yogakkhemo pavaḍḍhati;",
"ja272:6.3": "Kareyyattasamaṃ vuttiṃ,",
"ja272:6.4": "sabbakiccesu paṇḍito”.",
"ja272:7.1": "“Etha byagghā nivattavho,",
"ja272:7.2": "Paccupetha mahāvanaṃ;",
"ja272:7.3": "Mā vanaṃ chindi nibyagghaṃ,",
"ja272:7.4": "Byagghā māhesu nibbanā”ti.",
"ja272:8.1": "Byagghajātakaṃ dutiyaṃ."
}