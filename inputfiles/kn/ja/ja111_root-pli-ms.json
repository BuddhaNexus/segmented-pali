{
  "ja111:0.1": "Jātaka ",
  "ja111:0.2": "Ekakanipāta ",
  "ja111:0.3": "Haṁcivagga ",
  "ja111:0.4": "1. Gadrabhapañhajātaka ",
  "ja111:1.1": "“Haṁci tuvaṁ evamaññasi seyyo, ",
  "ja111:1.2": "Puttena pitāti rājaseṭṭha; ",
  "ja111:1.3": "Handassatarassa te ayaṁ, ",
  "ja111:1.4": "Assatarassa hi gadrabho pitā”ti. ",
  "ja111:2.1": "Gadrabhapañhajātakaṁ paṭhamaṁ. "
}