{
"ja111:1.1": "Jātaka",
"ja111:2.1": "Ekakanipāta",
"ja111:3.1": "Haṃcivagga",
"ja111:4.1": "1. Gadrabhapañhajātaka",
"ja111:5.1": "“Haṃci tuvaṃ evamaññasi seyyo,",
"ja111:5.2": "Puttena pitāti rājaseṭṭha;",
"ja111:5.3": "Handassatarassa te ayaṃ,",
"ja111:5.4": "Assatarassa hi gadrabho pitā”ti.",
"ja111:6.1": "Gadrabhapañhajātakaṃ paṭhamaṃ."
}