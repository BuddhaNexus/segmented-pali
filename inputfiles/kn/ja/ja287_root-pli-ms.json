{
"ja287:1.1": "Jātaka",
"ja287:2.1": "Tikanipāta",
"ja287:3.1": "Abbhantaravagga",
"ja287:4.1": "7. Lābhagarahajātaka",
"ja287:5.1": "“Nānummatto nāpisuṇo,",
"ja287:5.2": "nānaṭo nākutūhalo;",
"ja287:5.3": "Mūḷhesu labhate lābhaṃ,",
"ja287:5.4": "esā te anusāsanī”.",
"ja287:6.1": "“Dhiratthu taṃ yasalābhaṃ,",
"ja287:6.2": "dhanalābhañca brāhmaṇa;",
"ja287:6.3": "Yā vutti vinipātena,",
"ja287:6.4": "adhammacaraṇena vā.",
"ja287:7.1": "Api ce pattamādāya,",
"ja287:7.2": "anagāro paribbaje;",
"ja287:7.3": "Esāva jīvikā seyyo,",
"ja287:7.4": "yā cādhammena esanā”ti.",
"ja287:8.1": "Lābhagarahajātakaṃ sattamaṃ."
}