{
"ja289:1.1": "Jātaka",
"ja289:2.1": "Tikanipāta",
"ja289:3.1": "Abbhantaravagga",
"ja289:4.1": "9. Nānāchandajātaka",
"ja289:5.1": "“Nānāchandā mahārāja,",
"ja289:5.2": "ekāgāre vasāmase;",
"ja289:5.3": "Ahaṃ gāmavaraṃ icche,",
"ja289:5.4": "brāhmaṇī ca gavaṃ sataṃ”.",
"ja289:6.1": "Putto ca ājaññarathaṃ,",
"ja289:6.2": "kaññā ca maṇikuṇḍalaṃ;",
"ja289:6.3": "Yā cesā puṇṇikā jammī,",
"ja289:6.4": "udukkhalaṃbhikaṅkhati.",
"ja289:7.1": "“Brāhmaṇassa gāmavaraṃ,",
"ja289:7.2": "brāhmaṇiyā gavaṃ sataṃ;",
"ja289:7.3": "Puttassa ājaññarathaṃ,",
"ja289:7.4": "kaññāya maṇikuṇḍalaṃ;",
"ja289:7.5": "Yañcetaṃ puṇṇikaṃ jammiṃ,",
"ja289:7.6": "paṭipādethudukkhalan”ti.",
"ja289:8.1": "Nānāchandajātakaṃ navamaṃ."
}