{
  "ja289:0.1": "Jātaka ",
  "ja289:0.2": "Tikanipāta ",
  "ja289:0.3": "Abbhantaravagga ",
  "ja289:0.4": "9. Nānāchandajātaka ",
  "ja289:1.1": "“Nānāchandā mahārāja, ",
  "ja289:1.2": "ekāgāre vasāmase; ",
  "ja289:1.3": "Ahaṁ gāmavaraṁ icche, ",
  "ja289:1.4": "brāhmaṇī ca gavaṁ sataṁ”. ",
  "ja289:2.1": "Putto ca ājaññarathaṁ, ",
  "ja289:2.2": "kaññā ca maṇikuṇḍalaṁ; ",
  "ja289:2.3": "Yā cesā puṇṇikā jammī, ",
  "ja289:2.4": "udukkhalaṁbhikaṅkhati. ",
  "ja289:3.1": "“Brāhmaṇassa gāmavaraṁ, ",
  "ja289:3.2": "brāhmaṇiyā gavaṁ sataṁ; ",
  "ja289:3.3": "Puttassa ājaññarathaṁ, ",
  "ja289:3.4": "kaññāya maṇikuṇḍalaṁ; ",
  "ja289:3.5": "Yañcetaṁ puṇṇikaṁ jammiṁ, ",
  "ja289:3.6": "paṭipādethudukkhalan”ti. ",
  "ja289:4.1": "Nānāchandajātakaṁ navamaṁ. "
}