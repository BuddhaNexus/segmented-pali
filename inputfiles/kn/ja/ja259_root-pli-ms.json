{
"ja259:1.1": "Jātaka",
"ja259:2.1": "Tikanipāta",
"ja259:3.1": "Saṅkappavagga",
"ja259:4.1": "9. Tirīṭavacchajātaka",
"ja259:5.1": "“Nayimassa vijjāmayamatthi kiñci,",
"ja259:5.2": "Na bandhavo no pana te sahāyo;",
"ja259:5.3": "Atha kena vaṇṇena tirīṭavaccho,",
"ja259:5.4": "Tedaṇḍiko bhuñjati aggapiṇḍaṃ”.",
"ja259:6.1": "“Āpāsu me yuddhaparājitassa,",
"ja259:6.2": "Ekassa katvā vivanasmi ghore;",
"ja259:6.3": "Pasārayī kicchagatassa pāṇiṃ,",
"ja259:6.4": "Tenūdatāriṃ dukhasampareto.",
"ja259:7.1": "Etassa kiccena idhānupatto,",
"ja259:7.2": "Vesāyino visayā jīvaloke;",
"ja259:7.3": "Lābhāraho tāta tirīṭavaccho,",
"ja259:7.4": "Dethassa bhogaṃ yajathañca yaññan”ti.",
"ja259:8.1": "Tirīṭavacchajātakaṃ navamaṃ."
}