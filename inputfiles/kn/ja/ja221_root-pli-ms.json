{
"ja221:1.1": "Jātaka",
"ja221:2.1": "Dukanipāta",
"ja221:3.1": "Kāsāvavagga",
"ja221:4.1": "1. Kāsāvajātaka",
"ja221:5.1": "“Anikkasāvo kāsāvaṃ,",
"ja221:5.2": "yo vatthaṃ paridahissati;",
"ja221:5.3": "Apeto damasaccena,",
"ja221:5.4": "na so kāsāvamarahati.",
"ja221:6.1": "Yo ca vantakasāvassa,",
"ja221:6.2": "Sīlesu susamāhito;",
"ja221:6.3": "Upeto damasaccena,",
"ja221:6.4": "Sa ve kāsāvamarahatī”ti.",
"ja221:7.1": "Kāsāvajātakaṃ paṭhamaṃ."
}