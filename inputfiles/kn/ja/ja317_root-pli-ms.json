{
  "ja317:0.1": "Jātaka ",
  "ja317:0.2": "Catukkanipāta ",
  "ja317:0.3": "Pucimandavagga ",
  "ja317:0.4": "7. Matarodanajātaka ",
  "ja317:1.1": "“Mataṁ mataṁ evaṁ rodatha, ",
  "ja317:1.2": "Na hi taṁ rodatha yo marissati; ",
  "ja317:1.3": "Sabbepi sarīradhārino, ",
  "ja317:1.4": "Anupubbena jahanti jīvitaṁ. ",
  "ja317:2.1": "Devamanussā catuppadā, ",
  "ja317:2.2": "Pakkhigaṇā uragā ca bhogino; ",
  "ja317:2.3": "Samhi sarīre anissarā, ",
  "ja317:2.4": "Ramamānāva jahanti jīvitaṁ. ",
  "ja317:3.1": "Evaṁ calitaṁ asaṇṭhitaṁ, ",
  "ja317:3.2": "Sukhadukkhaṁ manujesvapekkhiya; ",
  "ja317:3.3": "Kanditaruditaṁ niratthakaṁ, ",
  "ja317:3.4": "Kiṁ vo sokagaṇābhikīrare. ",
  "ja317:4.1": "Dhuttā ca soṇḍā akatā, ",
  "ja317:4.2": "bālā sūrā ayogino; ",
  "ja317:4.3": "Dhīraṁ maññanti bāloti, ",
  "ja317:4.4": "ye dhammassa akovidā”ti. ",
  "ja317:5.1": "Matarodanajātakaṁ sattamaṁ. "
}