{
"ja109:1.1": "Jātaka",
"ja109:2.1": "Ekakanipāta",
"ja109:3.1": "Parosatavagga",
"ja109:4.1": "9. Kuṇḍapūvajātaka",
"ja109:5.1": "“Yathanno puriso hoti,",
"ja109:5.2": "tathannā tassa devatā;",
"ja109:5.3": "Āharetaṃ kuṇḍapūvaṃ,",
"ja109:5.4": "mā me bhāgaṃ vināsayā”ti.",
"ja109:6.1": "Kuṇḍapūvajātakaṃ navamaṃ."
}