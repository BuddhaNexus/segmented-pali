{
"ja236:1.1": "Jātaka",
"ja236:2.1": "Dukanipāta",
"ja236:3.1": "Upāhanavagga",
"ja236:4.1": "6. Bakajātaka",
"ja236:5.1": "“Bhaddako vatayaṃ pakkhī,",
"ja236:5.2": "dijo kumudasannibho;",
"ja236:5.3": "Vūpasantehi pakkhehi,",
"ja236:5.4": "mandamandova jhāyati”.",
"ja236:6.1": "“Nāssa sīlaṃ vijānātha,",
"ja236:6.2": "anaññāya pasaṃsatha;",
"ja236:6.3": "Amhe dijo na pāleti,",
"ja236:6.4": "tena pakkhī na phandatī”ti.",
"ja236:7.1": "Bakajātakaṃ chaṭṭhaṃ."
}