{
"ja41:1.1": "Jātaka",
"ja41:2.1": "Ekakanipāta",
"ja41:3.1": "Atthakāmavagga",
"ja41:4.1": "1. Losakajātaka",
"ja41:5.1": "“Yo atthakāmassa hitānukampino,",
"ja41:5.2": "Ovajjamāno na karoti sāsanaṃ;",
"ja41:5.3": "Ajiyā pādamolamba,",
"ja41:5.4": "Mittako viya socatī”ti.",
"ja41:6.1": "Losakajātakaṃ paṭhamaṃ."
}