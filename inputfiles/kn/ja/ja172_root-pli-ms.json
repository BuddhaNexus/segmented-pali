{
  "ja172:0.1": "Jātaka ",
  "ja172:0.2": "Dukanipāta ",
  "ja172:0.3": "Kalyāṇavagga ",
  "ja172:0.4": "2. Daddarajātaka ",
  "ja172:1.1": "“Ko nu saddena mahatā, ",
  "ja172:1.2": "abhinādeti daddaraṁ; ",
  "ja172:1.3": "Taṁ sīhā nappaṭinadanti, ",
  "ja172:1.4": "ko nāmeso migādhibhū”. ",
  "ja172:2.1": "“Adhamo migajātānaṁ, ",
  "ja172:2.2": "siṅgālo tāta vassati; ",
  "ja172:2.3": "Jātimassa jigucchantā, ",
  "ja172:2.4": "tuṇhī sīhā samacchare”ti. ",
  "ja172:3.1": "Daddarajātakaṁ dutiyaṁ. "
}