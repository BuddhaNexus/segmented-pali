{
"ja172:1.1": "Jātaka",
"ja172:2.1": "Dukanipāta",
"ja172:3.1": "Kalyāṇavagga",
"ja172:4.1": "2. Daddarajātaka",
"ja172:5.1": "“Ko nu saddena mahatā,",
"ja172:5.2": "abhinādeti daddaraṃ;",
"ja172:5.3": "Taṃ sīhā nappaṭinadanti,",
"ja172:5.4": "ko nāmeso migādhibhū”.",
"ja172:6.1": "“Adhamo migajātānaṃ,",
"ja172:6.2": "siṅgālo tāta vassati;",
"ja172:6.3": "Jātimassa jigucchantā,",
"ja172:6.4": "tuṇhī sīhā samacchare”ti.",
"ja172:7.1": "Daddarajātakaṃ dutiyaṃ."
}