{
"ja456:1.1": "Jātaka",
"ja456:2.1": "Ekādasakanipāta",
"ja456:3.1": "Mātuposakavagga",
"ja456:4.1": "2. Juṇhajātaka",
"ja456:5.1": "“Suṇohi mayhaṃ vacanaṃ janinda,",
"ja456:5.2": "Atthena juṇhamhi idhānupatto;",
"ja456:5.3": "Na brāhmaṇe addhike tiṭṭhamāne,",
"ja456:5.4": "Gantabbamāhu dvipadindaseṭṭha”.",
"ja456:6.1": "“Suṇomi tiṭṭhāmi vadehi brahme,",
"ja456:6.2": "Yenāsi atthena idhānupatto;",
"ja456:6.3": "Kaṃ vā tvamatthaṃ mayi patthayāno,",
"ja456:6.4": "Idhāgamā brahme tadiṅgha brūhi”.",
"ja456:7.1": "“Dadāhi me gāmavarāni pañca,",
"ja456:7.2": "Dāsīsataṃ satta gavaṃsatāni;",
"ja456:7.3": "Parosahassañca suvaṇṇanikkhe,",
"ja456:7.4": "Bhariyā ca me sādisī dve dadāhi.",
"ja456:8.1": "Tapo nu te brāhmaṇa bhiṃsarūpo,",
"ja456:8.2": "Mantā nu te brāhmaṇa cittarūpā;",
"ja456:8.3": "Yakkhā nu te assavā santi keci,",
"ja456:8.4": "Atthaṃ vā me abhijānāsi kattaṃ.",
"ja456:9.1": "Na me tapo atthi na cāpi mantā,",
"ja456:9.2": "Yakkhāpi me assavā natthi keci;",
"ja456:9.3": "Atthampi te nābhijānāmi kattaṃ,",
"ja456:9.4": "Pubbe ca kho saṅgatimattamāsi.",
"ja456:10.1": "Paṭhamaṃ idaṃ dassanaṃ jānato me,",
"ja456:10.2": "Na tābhijānāmi ito puratthā;",
"ja456:10.3": "Akkhāhi me pucchito etamatthaṃ,",
"ja456:10.4": "Kadā kuhiṃ vā ahu saṅgamo no.",
"ja456:11.1": "Gandhārarājassa puramhi ramme,",
"ja456:11.2": "Avasimhase takkasīlāyaṃ deva;",
"ja456:11.3": "Tatthandhakāramhi timīsikāyaṃ,",
"ja456:11.4": "Aṃsena aṃsaṃ samaghaṭṭayimha.",
"ja456:12.1": "Te tattha ṭhatvāna ubho janinda,",
"ja456:12.2": "Sārāṇiyaṃ vītisārayimha tattha;",
"ja456:12.3": "Sāyeva no saṅgatimattamāsi,",
"ja456:12.4": "Tato na pacchā na pure ahosi.",
"ja456:13.1": "Yadā kadāci manujesu brahme,",
"ja456:13.2": "Samāgamo sappurisena hoti;",
"ja456:13.3": "Na paṇḍitā saṅgatisanthavāni,",
"ja456:13.4": "Pubbe kataṃ vāpi vināsayanti.",
"ja456:14.1": "Bālāva kho saṅgatisanthavāni,",
"ja456:14.2": "Pubbe kataṃ vāpi vināsayanti;",
"ja456:14.3": "Bahumpi bālesu kataṃ vinassati,",
"ja456:14.4": "Tathā hi bālā akataññurūpā.",
"ja456:15.1": "Dhīrā ca kho saṅgatisanthavāni,",
"ja456:15.2": "Pubbe kataṃ vāpi na nāsayanti;",
"ja456:15.3": "Appampi dhīresu kataṃ na nassati,",
"ja456:15.4": "Tathā hi dhīrā sukataññurūpā.",
"ja456:16.1": "Dadāmi te gāmavarāni pañca,",
"ja456:16.2": "Dāsīsataṃ satta gavaṃsatāni;",
"ja456:16.3": "Parosahassañca suvaṇṇanikkhe,",
"ja456:16.4": "Bhariyā ca te sādisī dve dadāmi.",
"ja456:17.1": "Evaṃ sataṃ hoti samecca rāja,",
"ja456:17.2": "Nakkhattarājāriva tārakānaṃ;",
"ja456:17.3": "Āpūratī kāsipatī tathāhaṃ,",
"ja456:17.4": "Tayāpi me saṅgamo ajja laddho”ti.",
"ja456:18.1": "Juṇhajātakaṃ dutiyaṃ."
}