{
"ja147:1.1": "Jātaka",
"ja147:2.1": "Ekakanipāta",
"ja147:3.1": "Kakaṇṭakavagga",
"ja147:4.1": "7. Puppharattajātaka",
"ja147:5.1": "“Nayidaṃ dukkhaṃ aduṃ dukkhaṃ,",
"ja147:5.2": "Yaṃ maṃ tudati vāyaso;",
"ja147:5.3": "Yaṃ sāmā puppharattena,",
"ja147:5.4": "Kattikaṃ nānubhossatī”ti.",
"ja147:6.1": "Puppharattajātakaṃ sattamaṃ."
}