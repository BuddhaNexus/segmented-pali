{
"ja450:1.1": "Jātaka",
"ja450:2.1": "Dasakanipāta",
"ja450:3.1": "Catudvāravagga",
"ja450:4.1": "12. Bilārakosiyajātaka",
"ja450:5.1": "“Apacantāpi dicchanti,",
"ja450:5.2": "santo laddhāna bhojanaṃ;",
"ja450:5.3": "Kimeva tvaṃ pacamāno,",
"ja450:5.4": "yaṃ na dajjā na taṃ samaṃ.",
"ja450:6.1": "Maccherā ca pamādā ca,",
"ja450:6.2": "evaṃ dānaṃ na diyyati;",
"ja450:6.3": "Puññaṃ ākaṅkhamānena,",
"ja450:6.4": "deyyaṃ hoti vijānatā.",
"ja450:7.1": "Yasseva bhīto na dadāti maccharī,",
"ja450:7.2": "Tadevādadato bhayaṃ;",
"ja450:7.3": "Jighacchā ca pipāsā ca,",
"ja450:7.4": "Yassa bhāyati maccharī;",
"ja450:7.5": "Tameva bālaṃ phusati,",
"ja450:7.6": "Asmiṃ loke paramhi ca.",
"ja450:8.1": "Tasmā vineyya maccheraṃ,",
"ja450:8.2": "dajjā dānaṃ malābhibhū;",
"ja450:8.3": "Puññāni paralokasmiṃ,",
"ja450:8.4": "patiṭṭhā honti pāṇinaṃ”.",
"ja450:9.1": "“Duddadaṃ dadamānānaṃ,",
"ja450:9.2": "dukkaraṃ kamma kubbataṃ;",
"ja450:9.3": "Asanto nānukubbanti,",
"ja450:9.4": "sataṃ dhammo durannayo.",
"ja450:10.1": "Tasmā satañca asataṃ,",
"ja450:10.2": "nānā hoti ito gati;",
"ja450:10.3": "Asanto nirayaṃ yanti,",
"ja450:10.4": "santo saggaparāyaṇā”.",
"ja450:11.1": "“Appasmeke pavecchanti,",
"ja450:11.2": "bahuneke na dicchare;",
"ja450:11.3": "Appasmā dakkhiṇā dinnā,",
"ja450:11.4": "sahassena samaṃ mitā”.",
"ja450:12.1": "“Dhammaṃ care yopi samuñchakaṃ care,",
"ja450:12.2": "Dārañca posaṃ dadamappakasmiṃ;",
"ja450:12.3": "Sataṃ sahassānaṃ sahassayāginaṃ,",
"ja450:12.4": "Kalampi nāgghanti tathāvidhassa te”.",
"ja450:13.1": "“Kenesa yañño vipulo mahagghato,",
"ja450:13.2": "Samena dinnassa na agghameti;",
"ja450:13.3": "Kathaṃ sataṃ sahassānaṃ sahassayāginaṃ,",
"ja450:13.4": "Kalampi nāgghanti tathāvidhassa te”.",
"ja450:14.1": "“Dadanti heke visame niviṭṭhā,",
"ja450:14.2": "Chetvā vadhitvā atha socayitvā;",
"ja450:14.3": "Sā dakkhiṇā assumukhā sadaṇḍā,",
"ja450:14.4": "Samena dinnassa na agghameti;",
"ja450:14.5": "Evaṃ sataṃ sahassānaṃ sahassayāginaṃ,",
"ja450:14.6": "Kalampi nāgghanti tathāvidhassa te”ti.",
"ja450:15.1": "Bilārakosiyajātakaṃ dvādasamaṃ."
}