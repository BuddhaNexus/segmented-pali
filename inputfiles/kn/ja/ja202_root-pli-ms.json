{
"ja202:1.1": "Jātaka",
"ja202:2.1": "Dukanipāta",
"ja202:3.1": "Nataṃdaḷhavagga",
"ja202:4.1": "2. Keḷisīlajātaka",
"ja202:5.1": "“Haṃsā koñcā mayūrā ca,",
"ja202:5.2": "hatthayo pasadā migā;",
"ja202:5.3": "Sabbe sīhassa bhāyanti,",
"ja202:5.4": "natthi kāyasmi tulyatā.",
"ja202:6.1": "Evameva manussesu,",
"ja202:6.2": "daharo cepi paññavā;",
"ja202:6.3": "So hi tattha mahā hoti,",
"ja202:6.4": "neva bālo sarīravā”ti.",
"ja202:7.1": "Keḷisīlajātakaṃ dutiyaṃ."
}