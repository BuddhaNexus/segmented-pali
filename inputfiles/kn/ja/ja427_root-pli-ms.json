{
"ja427:1.1": "Jātaka",
"ja427:2.1": "Navakanipāta",
"ja427:3.1": "Gijjhavagga",
"ja427:4.1": "1. Gijjhajātaka",
"ja427:5.1": "“Parisaṅkupatho nāma,",
"ja427:5.2": "gijjhapantho sanantano;",
"ja427:5.3": "Tatrāsi mātāpitaro,",
"ja427:5.4": "gijjho posesi jiṇṇake;",
"ja427:5.5": "Tesaṃ ajagaramedaṃ,",
"ja427:5.6": "accahāsi bahuttaso.",
"ja427:6.1": "Pitā ca puttaṃ avaca,",
"ja427:6.2": "Jānaṃ uccaṃ papātinaṃ;",
"ja427:6.3": "Supattaṃ thāmasampannaṃ,",
"ja427:6.4": "Tejassiṃ dūragāminaṃ.",
"ja427:7.1": "Pariplavantaṃ pathaviṃ,",
"ja427:7.2": "yadā tāta vijānahi;",
"ja427:7.3": "Sāgarena parikkhittaṃ,",
"ja427:7.4": "cakkaṃva parimaṇḍalaṃ;",
"ja427:7.5": "Tato tāta nivattassu,",
"ja427:7.6": "māssu etto paraṃ gami.",
"ja427:8.1": "Udapattosi vegena,",
"ja427:8.2": "balī pakkhī dijuttamo;",
"ja427:8.3": "Olokayanto vakkaṅgo,",
"ja427:8.4": "pabbatāni vanāni ca.",
"ja427:9.1": "Addassa pathaviṃ gijjho,",
"ja427:9.2": "yathāsāsi pitussutaṃ;",
"ja427:9.3": "Sāgarena parikkhittaṃ,",
"ja427:9.4": "cakkaṃva parimaṇḍalaṃ.",
"ja427:10.1": "Tañca so samatikkamma,",
"ja427:10.2": "paramevaccavattatha;",
"ja427:10.3": "Tañca vātasikhā tikkhā,",
"ja427:10.4": "accahāsi baliṃ dijaṃ.",
"ja427:11.1": "Nāsakkhātigato poso,",
"ja427:11.2": "punadeva nivattituṃ;",
"ja427:11.3": "Dijo byasanamāpādi,",
"ja427:11.4": "verambhānaṃ vasaṃ gato.",
"ja427:12.1": "Tassa puttā ca dārā ca,",
"ja427:12.2": "ye caññe anujīvino;",
"ja427:12.3": "Sabbe byasanamāpāduṃ,",
"ja427:12.4": "anovādakare dije.",
"ja427:13.1": "Evampi idha vuḍḍhānaṃ,",
"ja427:13.2": "yo vākyaṃ nāvabujjhati;",
"ja427:13.3": "Atisīmacaro ditto,",
"ja427:13.4": "gijjhovātītasāsano;",
"ja427:13.5": "Sa ve byasanaṃ pappoti,",
"ja427:13.6": "akatvā vuḍḍhasāsanan”ti.",
"ja427:14.1": "Gijjhajātakaṃ paṭhamaṃ."
}