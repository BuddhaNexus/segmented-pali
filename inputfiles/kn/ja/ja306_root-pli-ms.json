{
"ja306:1.1": "Jātaka",
"ja306:2.1": "Catukkanipāta",
"ja306:3.1": "Kāliṅgavagga",
"ja306:4.1": "6. Sujātājātaka",
"ja306:5.1": "“Kimaṇḍakā ime deva,",
"ja306:5.2": "nikkhittā kaṃsamallake;",
"ja306:5.3": "Upalohitakā vaggū,",
"ja306:5.4": "taṃ me akkhāhi pucchito”.",
"ja306:6.1": "“Yāni pure tuvaṃ devi,",
"ja306:6.2": "bhaṇḍu nantakavāsinī;",
"ja306:6.3": "Ucchaṅgahatthā pacināsi,",
"ja306:6.4": "tassā te koliyaṃ phalaṃ.",
"ja306:7.1": "Uḍḍayhate na ramati,",
"ja306:7.2": "bhogā vippajahanti taṃ;",
"ja306:7.3": "Tatthevimaṃ paṭinetha,",
"ja306:7.4": "yattha kolaṃ pacissati”.",
"ja306:8.1": "“Honti hete mahārāja,",
"ja306:8.2": "iddhippattāya nāriyā;",
"ja306:8.3": "Khama deva sujātāya,",
"ja306:8.4": "māssā kujjha rathesabhā”ti.",
"ja306:9.1": "Sujātājātakaṃ chaṭṭhaṃ."
}