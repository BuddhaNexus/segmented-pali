{
"ja266:1.1": "Jātaka",
"ja266:2.1": "Tikanipāta",
"ja266:3.1": "Padumavagga",
"ja266:4.1": "6. Vātaggasindhavajātaka",
"ja266:5.1": "“Yenāsi kisiyā paṇḍu,",
"ja266:5.2": "yena bhattaṃ na ruccati;",
"ja266:5.3": "Ayaṃ so āgato bhattā,",
"ja266:5.4": "kasmā dāni palāyasi”.",
"ja266:6.1": "“Sace panādikeneva,",
"ja266:6.2": "santhavo nāma jāyati;",
"ja266:6.3": "Yaso hāyati itthīnaṃ,",
"ja266:6.4": "tasmā tāta palāyahaṃ”.",
"ja266:7.1": "Yasassinaṃ kule jātaṃ,",
"ja266:7.2": "āgataṃ yā na icchati;",
"ja266:7.3": "Socati cirarattāya,",
"ja266:7.4": "vātaggamiva bhaddalīti.",
"ja266:8.1": "Vātaggasindhavajātakaṃ chaṭṭhaṃ."
}