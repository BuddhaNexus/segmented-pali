{
"ja261:1.1": "Jātaka",
"ja261:2.1": "Tikanipāta",
"ja261:3.1": "Padumavagga",
"ja261:4.1": "1. Padumajātaka",
"ja261:5.1": "“Yathā kesā ca massū ca,",
"ja261:5.2": "chinnaṃ chinnaṃ virūhati;",
"ja261:5.3": "Evaṃ rūhatu te nāsā,",
"ja261:5.4": "padumaṃ dehi yācito”.",
"ja261:6.1": "“Yathā sāradikaṃ bījaṃ,",
"ja261:6.2": "khette vuttaṃ virūhati;",
"ja261:6.3": "Evaṃ rūhatu te nāsā,",
"ja261:6.4": "padumaṃ dehi yācito”.",
"ja261:7.1": "“Ubhopi palapantete,",
"ja261:7.2": "api padmāni dassati;",
"ja261:7.3": "Vajjuṃ vā te na vā vajjuṃ,",
"ja261:7.4": "natthi nāsāya rūhanā;",
"ja261:7.5": "Dehi samma padumāni,",
"ja261:7.6": "ahaṃ yācāmi yācito”ti.",
"ja261:8.1": "Padumajātakaṃ paṭhamaṃ."
}