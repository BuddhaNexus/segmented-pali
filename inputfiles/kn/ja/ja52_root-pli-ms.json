{
"ja52:1.1": "Jātaka",
"ja52:2.1": "Ekakanipāta",
"ja52:3.1": "Āsīsavagga",
"ja52:4.1": "2. Cūḷajanakajātaka",
"ja52:5.1": "“Vāyametheva puriso,",
"ja52:5.2": "na nibbindeyya paṇḍito;",
"ja52:5.3": "Passāmi vohaṃ attānaṃ,",
"ja52:5.4": "udakā thalamubbhatan”ti.",
"ja52:6.1": "Cūḷajanakajātakaṃ dutiyaṃ."
}