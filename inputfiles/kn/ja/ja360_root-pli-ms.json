{
"ja360:1.1": "Jātaka",
"ja360:2.1": "Pañcakanipāta",
"ja360:3.1": "Maṇikuṇḍalavagga",
"ja360:4.1": "10. Suyonandījātaka",
"ja360:5.1": "“Vāti gandho timirānaṃ,",
"ja360:5.2": "kusamuddo ca ghosavā;",
"ja360:5.3": "Dūre ito suyonandī,",
"ja360:5.4": "tamba kāmā tudanti maṃ”.",
"ja360:6.1": "“Kathaṃ samuddamatari,",
"ja360:6.2": "Kathaṃ addakkhi sedumaṃ;",
"ja360:6.3": "Kathaṃ tassā ca tuyhañca,",
"ja360:6.4": "Ahu saggasamāgamo”.",
"ja360:7.1": "“Kurukacchā payātānaṃ,",
"ja360:7.2": "vāṇijānaṃ dhanesinaṃ;",
"ja360:7.3": "Makarehi abhidā nāvā,",
"ja360:7.4": "phalakenāhamaplaviṃ.",
"ja360:8.1": "Sā maṃ saṇhena mudunā,",
"ja360:8.2": "niccaṃ candanagandhinī;",
"ja360:8.3": "Aṅgena uddharī bhaddā,",
"ja360:8.4": "mātā puttaṃva orasaṃ.",
"ja360:9.1": "Sā maṃ annena pānena,",
"ja360:9.2": "vatthena sayanena ca;",
"ja360:9.3": "Attanāpi ca mandakkhī,",
"ja360:9.4": "evaṃ tamba vijānahī”ti.",
"ja360:10.1": "Suyonandījātakaṃ dasamaṃ.",
"ja360:10.2": "Maṇikuṇḍalavaggo paṭhamo.",
"ja360:11.1": "Tassuddānaṃ",
"ja360:12.1": "Atha jinavaro haritaṃ tiṇako,",
"ja360:12.2": "Atha bhinnaplavo uragova ghaṭo;",
"ja360:12.3": "Dariyā puna kuñjara bhūnahatā,",
"ja360:12.4": "Migamuttamasaggavarena dasāti."
}