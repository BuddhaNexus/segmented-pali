{
  "ja360:0.1": "Jātaka ",
  "ja360:0.2": "Pañcakanipāta ",
  "ja360:0.3": "Maṇikuṇḍalavagga ",
  "ja360:0.4": "10. Suyonandījātaka ",
  "ja360:1.1": "“Vāti gandho timirānaṁ, ",
  "ja360:1.2": "kusamuddo ca ghosavā; ",
  "ja360:1.3": "Dūre ito suyonandī, ",
  "ja360:1.4": "tamba kāmā tudanti maṁ”. ",
  "ja360:2.1": "“Kathaṁ samuddamatari, ",
  "ja360:2.2": "Kathaṁ addakkhi sedumaṁ; ",
  "ja360:2.3": "Kathaṁ tassā ca tuyhañca, ",
  "ja360:2.4": "Ahu saggasamāgamo”. ",
  "ja360:3.1": "“Kurukacchā payātānaṁ, ",
  "ja360:3.2": "vāṇijānaṁ dhanesinaṁ; ",
  "ja360:3.3": "Makarehi abhidā nāvā, ",
  "ja360:3.4": "phalakenāhamaplaviṁ. ",
  "ja360:4.1": "Sā maṁ saṇhena mudunā, ",
  "ja360:4.2": "niccaṁ candanagandhinī; ",
  "ja360:4.3": "Aṅgena uddharī bhaddā, ",
  "ja360:4.4": "mātā puttaṁva orasaṁ. ",
  "ja360:5.1": "Sā maṁ annena pānena, ",
  "ja360:5.2": "vatthena sayanena ca; ",
  "ja360:5.3": "Attanāpi ca mandakkhī, ",
  "ja360:5.4": "evaṁ tamba vijānahī”ti. ",
  "ja360:6.1": "Suyonandījātakaṁ dasamaṁ. ",
  "ja360:6.2": "Maṇikuṇḍalavaggo paṭhamo. ",
  "ja360:7.1": "Tassuddānaṁ ",
  "ja360:8.1": "Atha jinavaro haritaṁ tiṇako, ",
  "ja360:8.2": "Atha bhinnaplavo uragova ghaṭo; ",
  "ja360:8.3": "Dariyā puna kuñjara bhūnahatā, ",
  "ja360:8.4": "Migamuttamasaggavarena dasāti. "
}