{
"ja193:1.1": "Jātaka",
"ja193:2.1": "Dukanipāta",
"ja193:3.1": "Ruhakavagga",
"ja193:4.1": "3. Cūḷapadumajātaka",
"ja193:5.1": "“Ayameva sā ahamapi so anañño,",
"ja193:5.2": "Ayameva so hatthacchinno anañño;",
"ja193:5.3": "Yamāha ‘komārapatī maman’ti,",
"ja193:5.4": "Vajjhitthiyo natthi itthīsu saccaṃ.",
"ja193:6.1": "Imañca jammaṃ musalena hantvā,",
"ja193:6.2": "Luddaṃ chavaṃ paradārūpaseviṃ;",
"ja193:6.3": "Imissā ca naṃ pāpapatibbatāya,",
"ja193:6.4": "Jīvantiyā chindatha kaṇṇanāsan”ti.",
"ja193:7.1": "Cūḷapadumajātakaṃ tatiyaṃ."
}