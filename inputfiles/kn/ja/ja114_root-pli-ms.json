{
"ja114:1.1": "Jātaka",
"ja114:2.1": "Ekakanipāta",
"ja114:3.1": "Haṃcivagga",
"ja114:4.1": "4. Mitacintijātaka",
"ja114:5.1": "“Bahucintī appacintī,",
"ja114:5.2": "ubho jāle abajjhare;",
"ja114:5.3": "Mitacintī pamocesī,",
"ja114:5.4": "ubho tattha samāgatā”ti.",
"ja114:6.1": "Mitacintijātakaṃ catutthaṃ."
}