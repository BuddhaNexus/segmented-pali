{
"ja495:1.1": "Jātaka",
"ja495:2.1": "Pakiṇṇakanipāta",
"ja495:3.1": "Sālikedāravagga",
"ja495:4.1": "12. Dasabrāhmaṇajātaka",
"ja495:5.1": "Rājā avoca vidhuraṃ,",
"ja495:5.2": "Dhammakāmo yudhiṭṭhilo;",
"ja495:5.3": "“Brāhmaṇe vidhura pariyesa,",
"ja495:5.4": "Sīlavante bahussute.",
"ja495:6.1": "Virate methunā dhammā,",
"ja495:6.2": "Ye me bhuñjeyyu bhojanaṃ;",
"ja495:6.3": "Dakkhiṇaṃ samma dassāma,",
"ja495:6.4": "Yattha dinnaṃ mahapphalaṃ”.",
"ja495:7.1": "“Dullabhā brāhmaṇā deva,",
"ja495:7.2": "sīlavanto bahussutā;",
"ja495:7.3": "Viratā methunā dhammā,",
"ja495:7.4": "ye te bhuñjeyyu bhojanaṃ.",
"ja495:8.1": "Dasa khalu mahārāja,",
"ja495:8.2": "yā tā brāhmaṇajātiyo;",
"ja495:8.3": "Tesaṃ vibhaṅgaṃ vicayaṃ,",
"ja495:8.4": "vitthārena suṇohi me.",
"ja495:9.1": "Pasibbake gahetvāna,",
"ja495:9.2": "puṇṇe mūlassa saṃvute;",
"ja495:9.3": "Osadhikāyo ganthenti,",
"ja495:9.4": "nhāpayanti japanti ca.",
"ja495:10.1": "Tikicchakasamā rāja,",
"ja495:10.2": "tepi vuccanti brāhmaṇā;",
"ja495:10.3": "Akkhātā te mahārāja,",
"ja495:10.4": "tādise nipatāmase”. #1",
"ja495:11.1": "“Apetā te ca brahmaññā,",
"ja495:11.2": "iti rājā korabyo",
"ja495:11.3": "Na te vuccanti brāhmaṇā;",
"ja495:11.4": "Aññe vidhura pariyesa,",
"ja495:11.5": "Sīlavante bahussute.",
"ja495:12.1": "Virate methunā dhammā,",
"ja495:12.2": "Ye me bhuñjeyyu bhojanaṃ;",
"ja495:12.3": "Dakkhiṇaṃ samma dassāma,",
"ja495:12.4": "Yattha dinnaṃ mahapphalaṃ”.",
"ja495:13.1": "“Kiṅkiṇikāyo gahetvā,",
"ja495:13.2": "ghosenti puratopi te;",
"ja495:13.3": "Pesanānipi gacchanti,",
"ja495:13.4": "rathacariyāsu sikkhare.",
"ja495:14.1": "Paricārakasamā rāja,",
"ja495:14.2": "tepi vuccanti brāhmaṇā;",
"ja495:14.3": "Akkhātā te mahārāja,",
"ja495:14.4": "tādise nipatāmase”. #2",
"ja495:15.1": "“Apetā te ca brahmaññā,",
"ja495:15.2": "iti rājā korabyo",
"ja495:15.3": "Na te vuccanti brāhmaṇā;",
"ja495:15.4": "Aññe vidhura pariyesa,",
"ja495:15.5": "Sīlavante bahussute.",
"ja495:16.1": "Virate methunā dhammā,",
"ja495:16.2": "ye me bhuñjeyyu bhojanaṃ;",
"ja495:16.3": "Dakkhiṇaṃ samma dassāma,",
"ja495:16.4": "yattha dinnaṃ mahapphalaṃ”.",
"ja495:17.1": "“Kamaṇḍaluṃ gahetvāna,",
"ja495:17.2": "vaṅkadaṇḍañca brāhmaṇā;",
"ja495:17.3": "Paccupessanti rājāno,",
"ja495:17.4": "gāmesu nigamesu ca;",
"ja495:17.5": "Nādinne vuṭṭhahissāma,",
"ja495:17.6": "gāmamhi vā vanamhi vā.",
"ja495:18.1": "Niggāhakasamā rāja,",
"ja495:18.2": "tepi vuccanti brāhmaṇā;",
"ja495:18.3": "Akkhātā te mahārāja,",
"ja495:18.4": "tādise nipatāmase”. #3",
"ja495:19.1": "“Apetā te ca brahmaññā,",
"ja495:19.2": "iti rājā korabyo",
"ja495:19.3": "Na te vuccanti brāhmaṇā;",
"ja495:19.4": "Aññe vidhura pariyesa,",
"ja495:19.5": "Sīlavante bahussute.",
"ja495:20.1": "Virate methunā dhammā,",
"ja495:20.2": "ye me bhuñjeyyu bhojanaṃ;",
"ja495:20.3": "Dakkhiṇaṃ samma dassāma,",
"ja495:20.4": "yattha dinnaṃ mahapphalaṃ”.",
"ja495:21.1": "“Parūḷhakacchanakhalomā,",
"ja495:21.2": "paṅkadantā rajassirā;",
"ja495:21.3": "Okiṇṇā rajareṇūhi,",
"ja495:21.4": "yācakā vicaranti te.",
"ja495:22.1": "Khāṇughātasamā rāja,",
"ja495:22.2": "tepi vuccanti brāhmaṇā;",
"ja495:22.3": "Akkhātā te mahārāja,",
"ja495:22.4": "tādise nipatāmase”. #4",
"ja495:23.1": "“Apetā te ca brahmaññā,",
"ja495:23.2": "iti rājā korabyo",
"ja495:23.3": "Na te vuccanti brāhmaṇā;",
"ja495:23.4": "Aññe vidhura pariyesa,",
"ja495:23.5": "Sīlavante bahussute.",
"ja495:24.1": "Virate methunā dhammā,",
"ja495:24.2": "ye me bhuñjeyyu bhojanaṃ;",
"ja495:24.3": "Dakkhiṇaṃ samma dassāma,",
"ja495:24.4": "yattha dinnaṃ mahapphalaṃ”.",
"ja495:25.1": "“Harītakaṃ āmalakaṃ,",
"ja495:25.2": "Ambaṃ jambuṃ vibhītakaṃ;",
"ja495:25.3": "Labujaṃ dantapoṇāni,",
"ja495:25.4": "Beluvā badarāni ca.",
"ja495:26.1": "Rājāyatanaṃ ucchupuṭaṃ,",
"ja495:26.2": "dhūmanettaṃ madhuañjanaṃ;",
"ja495:26.3": "Uccāvacāni paṇiyāni,",
"ja495:26.4": "vipaṇenti janādhipa.",
"ja495:27.1": "Vāṇijakasamā rāja,",
"ja495:27.2": "tepi vuccanti brāhmaṇā;",
"ja495:27.3": "Akkhātā te mahārāja,",
"ja495:27.4": "tādise nipatāmase”. #5",
"ja495:28.1": "“Apetā te ca brahmaññā,",
"ja495:28.2": "iti rājā korabyo",
"ja495:28.3": "Na te vuccanti brāhmaṇā;",
"ja495:28.4": "Aññe vidhura pariyesa,",
"ja495:28.5": "Sīlavante bahussute.",
"ja495:29.1": "Virate methunā dhammā,",
"ja495:29.2": "ye me bhuñjeyyu bhojanaṃ;",
"ja495:29.3": "Dakkhiṇaṃ samma dassāma,",
"ja495:29.4": "yattha dinnaṃ mahapphalaṃ”.",
"ja495:30.1": "“Kasivāṇijjaṃ kārenti,",
"ja495:30.2": "posayanti ajeḷake;",
"ja495:30.3": "Kumāriyo pavecchanti,",
"ja495:30.4": "vivāhantāvahanti ca.",
"ja495:31.1": "Samā ambaṭṭhavessehi,",
"ja495:31.2": "tepi vuccanti brāhmaṇā;",
"ja495:31.3": "Akkhātā te mahārāja,",
"ja495:31.4": "tādise nipatāmase”. #6",
"ja495:32.1": "“Apetā te ca brahmaññā,",
"ja495:32.2": "iti rājā korabyo",
"ja495:32.3": "Na te vuccanti brāhmaṇā;",
"ja495:32.4": "Aññe vidhura pariyesa,",
"ja495:32.5": "Sīlavante bahussute.",
"ja495:33.1": "Virate methunā dhammā,",
"ja495:33.2": "ye me bhuñjeyyu bhojanaṃ;",
"ja495:33.3": "Dakkhiṇaṃ samma dassāma,",
"ja495:33.4": "yattha dinnaṃ mahapphalaṃ”.",
"ja495:34.1": "“Nikkhittabhikkhaṃ bhuñjanti,",
"ja495:34.2": "gāmesveke purohitā;",
"ja495:34.3": "Bahū te paripucchanti,",
"ja495:34.4": "aṇḍacchedā nilañchakā.",
"ja495:35.1": "Pasūpi tattha haññanti,",
"ja495:35.2": "mahiṃsā sūkarā ajā;",
"ja495:35.3": "Goghātakasamā rāja,",
"ja495:35.4": "tepi vuccanti brāhmaṇā;",
"ja495:35.5": "Akkhātā te mahārāja,",
"ja495:35.6": "tādise nipatāmase”. #7",
"ja495:36.1": "“Apetā te ca brahmaññā,",
"ja495:36.2": "iti rājā korabyo",
"ja495:36.3": "Na te vuccanti brāhmaṇā;",
"ja495:36.4": "Aññe vidhura pariyesa,",
"ja495:36.5": "Sīlavante bahussute.",
"ja495:37.1": "Virate methunā dhammā,",
"ja495:37.2": "ye me bhuñjeyyu bhojanaṃ;",
"ja495:37.3": "Dakkhiṇaṃ samma dassāma,",
"ja495:37.4": "yattha dinnaṃ mahapphalaṃ”.",
"ja495:38.1": "“Asicammaṃ gahetvāna,",
"ja495:38.2": "khaggaṃ paggayha brāhmaṇā;",
"ja495:38.3": "Vessapathesu tiṭṭhanti,",
"ja495:38.4": "satthaṃ abbāhayantipi.",
"ja495:39.1": "Samā gopanisādehi,",
"ja495:39.2": "tepi vuccanti brāhmaṇā;",
"ja495:39.3": "Akkhātā te mahārāja,",
"ja495:39.4": "tādise nipatāmase”. #8",
"ja495:40.1": "“Apetā te ca brahmaññā,",
"ja495:40.2": "iti rājā korabyo",
"ja495:40.3": "Na te vuccanti brāhmaṇā;",
"ja495:40.4": "Aññe vidhura pariyesa,",
"ja495:40.5": "Sīlavante bahussute.",
"ja495:41.1": "Virate methunā dhammā,",
"ja495:41.2": "ye me bhuñjeyyu bhojanaṃ;",
"ja495:41.3": "Dakkhiṇaṃ samma dassāma,",
"ja495:41.4": "yattha dinnaṃ mahapphalaṃ”.",
"ja495:42.1": "“Araññe kuṭikaṃ katvā,",
"ja495:42.2": "kūṭāni kārayanti te;",
"ja495:42.3": "Sasabiḷāre bādhenti,",
"ja495:42.4": "āgodhā macchakacchapaṃ.",
"ja495:43.1": "Te luddakasamā rāja,",
"ja495:43.2": "tepi vuccanti brāhmaṇā;",
"ja495:43.3": "Akkhātā te mahārāja,",
"ja495:43.4": "tādise nipatāmase”. #9",
"ja495:44.1": "“Apetā te ca brahmaññā,",
"ja495:44.2": "iti rājā korabyo",
"ja495:44.3": "Na te vuccanti brāhmaṇā;",
"ja495:44.4": "Aññe vidhura pariyesa,",
"ja495:44.5": "Sīlavante bahussute.",
"ja495:45.1": "Virate methunā dhammā,",
"ja495:45.2": "ye me bhuñjeyyu bhojanaṃ;",
"ja495:45.3": "Dakkhiṇaṃ samma dassāma,",
"ja495:45.4": "yattha dinnaṃ mahapphalaṃ”.",
"ja495:46.1": "“Aññe dhanassa kāmā hi,",
"ja495:46.2": "heṭṭhāmañce pasakkitā;",
"ja495:46.3": "Rājāno upari nhāyanti,",
"ja495:46.4": "somayāge upaṭṭhite.",
"ja495:47.1": "Malamajjakasamā rāja,",
"ja495:47.2": "Tepi vuccanti brāhmaṇā;",
"ja495:47.3": "Akkhātā te mahārāja,",
"ja495:47.4": "Tādise nipatāmase”. #10",
"ja495:48.1": "“Apetā te ca brahmaññā,",
"ja495:48.2": "iti rājā korabyo",
"ja495:48.3": "Na te vuccanti brāhmaṇā;",
"ja495:48.4": "Aññe vidhura pariyesa,",
"ja495:48.5": "Sīlavante bahussute.",
"ja495:49.1": "Virate methunā dhammā,",
"ja495:49.2": "ye me bhuñjeyyu bhojanaṃ;",
"ja495:49.3": "Dakkhiṇaṃ samma dassāma,",
"ja495:49.4": "yattha dinnaṃ mahapphalaṃ”.",
"ja495:50.1": "“Atthi kho brāhmaṇā deva,",
"ja495:50.2": "sīlavanto bahussutā;",
"ja495:50.3": "Viratā methunā dhammā,",
"ja495:50.4": "ye te bhuñjeyyu bhojanaṃ.",
"ja495:51.1": "Ekañca bhattaṃ bhuñjanti,",
"ja495:51.2": "na ca majjaṃ pivanti te;",
"ja495:51.3": "Akkhātā te mahārāja,",
"ja495:51.4": "tādise nipatāmase”.",
"ja495:52.1": "“Ete kho brāhmaṇā vidhura,",
"ja495:52.2": "Sīlavanto bahussutā;",
"ja495:52.3": "Ete vidhura pariyesa,",
"ja495:52.4": "Khippañca ne nimantayā”ti.",
"ja495:53.1": "Dasabrāhmaṇajātakaṃ dvādasamaṃ."
}