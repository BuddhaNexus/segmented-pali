{
  "ja237:0.1": "Jātaka ",
  "ja237:0.2": "Dukanipāta ",
  "ja237:0.3": "Upāhanavagga ",
  "ja237:0.4": "7. Sāketajātaka ",
  "ja237:1.1": "“Ko nu kho bhagavā hetu, ",
  "ja237:1.2": "ekacce idha puggale; ",
  "ja237:1.3": "Atīva hadayaṁ nibbāti, ",
  "ja237:1.4": "cittañcāpi pasīdati”. ",
  "ja237:2.1": "“Pubbeva sannivāsena, ",
  "ja237:2.2": "paccuppannahitena vā; ",
  "ja237:2.3": "Evaṁ taṁ jāyate pemaṁ, ",
  "ja237:2.4": "uppalaṁva yathodake”ti. ",
  "ja237:3.1": "Sāketajātakaṁ sattamaṁ. "
}