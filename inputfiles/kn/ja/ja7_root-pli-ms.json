{
"ja7:1.1": "Jātaka",
"ja7:2.1": "Ekakanipāta",
"ja7:3.1": "Apaṇṇakavagga",
"ja7:4.1": "7. Kaṭṭhahārijātaka",
"ja7:5.1": "“Putto tyāhaṃ mahārāja,",
"ja7:5.2": "tvaṃ maṃ posa janādhipa;",
"ja7:5.3": "Aññepi devo poseti,",
"ja7:5.4": "kiñca devo sakaṃ pajan”ti.",
"ja7:6.1": "Kaṭṭhahārijātakaṃ sattamaṃ."
}