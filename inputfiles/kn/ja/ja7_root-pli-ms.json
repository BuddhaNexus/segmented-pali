{
  "ja7:0.1": "Jātaka ",
  "ja7:0.2": "Ekakanipāta ",
  "ja7:0.3": "Apaṇṇakavagga ",
  "ja7:0.4": "7. Kaṭṭhahārijātaka ",
  "ja7:1.1": "“Putto tyāhaṁ mahārāja, ",
  "ja7:1.2": "tvaṁ maṁ posa janādhipa; ",
  "ja7:1.3": "Aññepi devo poseti, ",
  "ja7:1.4": "kiñca devo sakaṁ pajan”ti. ",
  "ja7:2.1": "Kaṭṭhahārijātakaṁ sattamaṁ. "
}