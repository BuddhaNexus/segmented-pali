{
"ja190:1.1": "Jātaka",
"ja190:2.1": "Dukanipāta",
"ja190:3.1": "Asadisavagga",
"ja190:4.1": "10. Sīlānisaṃsajātaka",
"ja190:5.1": "“Passa saddhāya sīlassa,",
"ja190:5.2": "cāgassa ca ayaṃ phalaṃ;",
"ja190:5.3": "Nāgo nāvāya vaṇṇena,",
"ja190:5.4": "saddhaṃ vahatupāsakaṃ.",
"ja190:6.1": "Sabbhireva samāsetha,",
"ja190:6.2": "sabbhi kubbetha santhavaṃ;",
"ja190:6.3": "Satañhi sannivāsena,",
"ja190:6.4": "sotthiṃ gacchati nhāpito”ti.",
"ja190:7.1": "Sīlānisaṃsajātakaṃ dasamaṃ.",
"ja190:7.2": "Asadisavaggo catuttho.",
"ja190:8.1": "Tassuddānaṃ",
"ja190:9.1": "Dhanuggaha kuñjara apparaso,",
"ja190:9.2": "Giridattamanāvilacittavaraṃ;",
"ja190:9.3": "Dadhivāhana jambūka sīhanakho,",
"ja190:9.4": "Haritayava nāgavarena dasāti."
}