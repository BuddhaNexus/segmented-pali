{
"ja200:1.1": "Jātaka",
"ja200:2.1": "Dukanipāta",
"ja200:3.1": "Ruhakavagga",
"ja200:4.1": "10. Sādhusīlajātaka",
"ja200:5.1": "“Sarīradabyaṃ vuḍḍhabyaṃ,",
"ja200:5.2": "sojaccaṃ sādhusīliyaṃ;",
"ja200:5.3": "Brāhmaṇaṃ teva pucchāma,",
"ja200:5.4": "kaṃ nu tesaṃ vanimhase”.",
"ja200:6.1": "“Attho atthi sarīrasmiṃ,",
"ja200:6.2": "vuḍḍhabyassa namo kare;",
"ja200:6.3": "Attho atthi sujātasmiṃ,",
"ja200:6.4": "sīlaṃ asmāka ruccatī”ti.",
"ja200:7.1": "Sādhusīlajātakaṃ dasamaṃ.",
"ja200:7.2": "Ruhakavaggo pañcamo.",
"ja200:8.1": "Tassuddānaṃ",
"ja200:9.1": "Apiruhaka rūpavatī musalo,",
"ja200:9.2": "Pavasanti sapañcamapokkharaṇī;",
"ja200:9.3": "Atha muttimavāṇija umhayate,",
"ja200:9.4": "Ciraāgata koṭṭha sarīra dasāti."
}