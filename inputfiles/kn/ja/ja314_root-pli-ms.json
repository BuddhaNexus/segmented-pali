{
  "ja314:0.1": "Jātaka ",
  "ja314:0.2": "Catukkanipāta ",
  "ja314:0.3": "Pucimandavagga ",
  "ja314:0.4": "4. Lohakumbhijātaka ",
  "ja314:1.1": "“Dujjīvitamajīvimha, ",
  "ja314:1.2": "ye sante na dadamhase; ",
  "ja314:1.3": "Vijjamānesu bhogesu, ",
  "ja314:1.4": "dīpaṁ nākamha attano”. ",
  "ja314:2.1": "“Saṭṭhi vassasahassāni, ",
  "ja314:2.2": "paripuṇṇāni sabbaso; ",
  "ja314:2.3": "Niraye paccamānānaṁ, ",
  "ja314:2.4": "kadā anto bhavissati”. ",
  "ja314:3.1": "“Natthi anto kuto anto, ",
  "ja314:3.2": "na anto paṭidissati; ",
  "ja314:3.3": "Tadā hi pakataṁ pāpaṁ, ",
  "ja314:3.4": "mama tuyhañca mārisā”. ",
  "ja314:4.1": "“Sohaṁ nūna ito gantvā, ",
  "ja314:4.2": "yoniṁ laddhāna mānusiṁ; ",
  "ja314:4.3": "Vadaññū sīlasampanno, ",
  "ja314:4.4": "kāhāmi kusalaṁ bahun”ti. ",
  "ja314:5.1": "Lohakumbhijātakaṁ catutthaṁ. "
}