{
  "ja226:0.1": "Jātaka ",
  "ja226:0.2": "Dukanipāta ",
  "ja226:0.3": "Kāsāvavagga ",
  "ja226:0.4": "6. Kosiyajātaka ",
  "ja226:1.1": "“Kāle nikkhamanā sādhu, ",
  "ja226:1.2": "nākāle sādhu nikkhamo; ",
  "ja226:1.3": "Akālena hi nikkhamma, ",
  "ja226:1.4": "ekakampi bahujjano; ",
  "ja226:1.5": "Na kiñci atthaṁ joteti, ",
  "ja226:1.6": "dhaṅkasenāva kosiyaṁ. ",
  "ja226:2.1": "Dhīro ca vidhividhānaññū, ",
  "ja226:2.2": "paresaṁ vivarānugū; ",
  "ja226:2.3": "Sabbāmitte vasīkatvā, ",
  "ja226:2.4": "kosiyova sukhī siyā”ti. ",
  "ja226:3.1": "Kosiyajātakaṁ chaṭṭhaṁ. "
}