{
"ja343:1.1": "Jātaka",
"ja343:2.1": "Catukkanipāta",
"ja343:3.1": "Cūḷakuṇālavagga",
"ja343:4.1": "3. Kuntinījātaka",
"ja343:5.1": "“Avasimha tavāgāre,",
"ja343:5.2": "niccaṃ sakkatapūjitā;",
"ja343:5.3": "Tvameva dānimakari,",
"ja343:5.4": "handa rāja vajāmahaṃ”.",
"ja343:6.1": "“Yo ve kate paṭikate,",
"ja343:6.2": "kibbise paṭikibbise;",
"ja343:6.3": "Evaṃ taṃ sammatī veraṃ,",
"ja343:6.4": "vasa kuntini māgamā”.",
"ja343:7.1": "“Na katassa ca kattā ca,",
"ja343:7.2": "metti sandhīyate puna;",
"ja343:7.3": "Hadayaṃ nānujānāti,",
"ja343:7.4": "gacchaññeva rathesabha”.",
"ja343:8.1": "“Katassa ceva kattā ca,",
"ja343:8.2": "metti sandhīyate puna;",
"ja343:8.3": "Dhīrānaṃ no ca bālānaṃ,",
"ja343:8.4": "vasa kuntini māgamā”ti.",
"ja343:9.1": "Kuntinījātakaṃ tatiyaṃ."
}