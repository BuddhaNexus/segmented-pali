{
  "ja343:0.1": "Jātaka ",
  "ja343:0.2": "Catukkanipāta ",
  "ja343:0.3": "Cūḷakuṇālavagga ",
  "ja343:0.4": "3. Kuntinījātaka ",
  "ja343:1.1": "“Avasimha tavāgāre, ",
  "ja343:1.2": "niccaṁ sakkatapūjitā; ",
  "ja343:1.3": "Tvameva dānimakari, ",
  "ja343:1.4": "handa rāja vajāmahaṁ”. ",
  "ja343:2.1": "“Yo ve kate paṭikate, ",
  "ja343:2.2": "kibbise paṭikibbise; ",
  "ja343:2.3": "Evaṁ taṁ sammatī veraṁ, ",
  "ja343:2.4": "vasa kuntini māgamā”. ",
  "ja343:3.1": "“Na katassa ca kattā ca, ",
  "ja343:3.2": "metti sandhīyate puna; ",
  "ja343:3.3": "Hadayaṁ nānujānāti, ",
  "ja343:3.4": "gacchaññeva rathesabha”. ",
  "ja343:4.1": "“Katassa ceva kattā ca, ",
  "ja343:4.2": "metti sandhīyate puna; ",
  "ja343:4.3": "Dhīrānaṁ no ca bālānaṁ, ",
  "ja343:4.4": "vasa kuntini māgamā”ti. ",
  "ja343:5.1": "Kuntinījātakaṁ tatiyaṁ. "
}