{
  "ja192:0.1": "Jātaka ",
  "ja192:0.2": "Dukanipāta ",
  "ja192:0.3": "Ruhakavagga ",
  "ja192:0.4": "2. Sirikāḷakaṇṇijātaka ",
  "ja192:1.1": "“Itthī siyā rūpavatī, ",
  "ja192:1.2": "sā ca sīlavatī siyā; ",
  "ja192:1.3": "Puriso taṁ na iccheyya, ",
  "ja192:1.4": "saddahāsi mahosadha”. ",
  "ja192:2.1": "“Saddahāmi mahārāja, ",
  "ja192:2.2": "puriso dubbhago siyā; ",
  "ja192:2.3": "Sirī ca kāḷakaṇṇī ca, ",
  "ja192:2.4": "na samenti kudācanan”ti. ",
  "ja192:3.1": "Sirikāḷakaṇṇijātakaṁ dutiyaṁ. "
}