{
"ja335:1.1": "Jātaka",
"ja335:2.1": "Catukkanipāta",
"ja335:3.1": "Kokilavagga",
"ja335:4.1": "5. Jambukajātaka",
"ja335:5.1": "“Brahā pavaḍḍhakāyo so,",
"ja335:5.2": "dīghadāṭho ca jambuka;",
"ja335:5.3": "Na tvaṃ tattha kule jāto,",
"ja335:5.4": "yattha gaṇhanti kuñjaraṃ”.",
"ja335:6.1": "“Asīho sīhamānena,",
"ja335:6.2": "yo attānaṃ vikubbati;",
"ja335:6.3": "Kotthūva gajamāsajja,",
"ja335:6.4": "seti bhūmyā anutthunaṃ.",
"ja335:7.1": "Yasassino uttamapuggalassa,",
"ja335:7.2": "Sañjātakhandhassa mahabbalassa;",
"ja335:7.3": "Asamekkhiya thāmabalūpapattiṃ,",
"ja335:7.4": "Sa seti nāgena hatoyaṃ jambuko.",
"ja335:8.1": "Yo cīdha kammaṃ kurute pamāya,",
"ja335:8.2": "Thāmabbalaṃ attani saṃviditvā;",
"ja335:8.3": "Jappena mantena subhāsitena,",
"ja335:8.4": "Parikkhavā so vipulaṃ jinātī”ti.",
"ja335:9.1": "Jambukajātakaṃ pañcamaṃ."
}