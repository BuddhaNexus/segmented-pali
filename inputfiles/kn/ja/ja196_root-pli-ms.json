{
  "ja196:0.1": "Jātaka ",
  "ja196:0.2": "Dukanipāta ",
  "ja196:0.3": "Ruhakavagga ",
  "ja196:0.4": "6. Valāhakassajātaka ",
  "ja196:1.1": "“Ye na kāhanti ovādaṁ, ",
  "ja196:1.2": "narā buddhena desitaṁ; ",
  "ja196:1.3": "Byasanaṁ te gamissanti, ",
  "ja196:1.4": "rakkhasīhiva vāṇijā. ",
  "ja196:2.1": "Ye ca kāhanti ovādaṁ, ",
  "ja196:2.2": "narā buddhena desitaṁ; ",
  "ja196:2.3": "Sotthiṁ pāraṁ gamissanti, ",
  "ja196:2.4": "valāheneva vāṇijā”ti. ",
  "ja196:3.1": "Valāhakassajātakaṁ chaṭṭhaṁ. "
}