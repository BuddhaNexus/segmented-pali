{
"ja196:1.1": "Jātaka",
"ja196:2.1": "Dukanipāta",
"ja196:3.1": "Ruhakavagga",
"ja196:4.1": "6. Valāhakassajātaka",
"ja196:5.1": "“Ye na kāhanti ovādaṃ,",
"ja196:5.2": "narā buddhena desitaṃ;",
"ja196:5.3": "Byasanaṃ te gamissanti,",
"ja196:5.4": "rakkhasīhiva vāṇijā.",
"ja196:6.1": "Ye ca kāhanti ovādaṃ,",
"ja196:6.2": "narā buddhena desitaṃ;",
"ja196:6.3": "Sotthiṃ pāraṃ gamissanti,",
"ja196:6.4": "valāheneva vāṇijā”ti.",
"ja196:7.1": "Valāhakassajātakaṃ chaṭṭhaṃ."
}