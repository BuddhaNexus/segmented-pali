{
"ja327:1.1": "Jātaka",
"ja327:2.1": "Catukkanipāta",
"ja327:3.1": "Kuṭidūsakavagga",
"ja327:4.1": "7. Kākavatījātaka",
"ja327:5.1": "“Vāti cāyaṃ tato gandho,",
"ja327:5.2": "yattha me vasatī piyā;",
"ja327:5.3": "Dūre ito hi kākavatī,",
"ja327:5.4": "yattha me nirato mano”.",
"ja327:6.1": "“Kathaṃ samuddamatarī,",
"ja327:6.2": "kathaṃ atari kepukaṃ;",
"ja327:6.3": "Kathaṃ satta samuddāni,",
"ja327:6.4": "kathaṃ simbalimāruhi”.",
"ja327:7.1": "“Tayā samuddamatariṃ,",
"ja327:7.2": "tayā atari kepukaṃ;",
"ja327:7.3": "Tayā satta samuddāni,",
"ja327:7.4": "tayā simbalimāruhiṃ”.",
"ja327:8.1": "“Dhiratthumaṃ mahākāyaṃ,",
"ja327:8.2": "dhiratthumaṃ acetanaṃ;",
"ja327:8.3": "Yattha jāyāyahaṃ jāraṃ,",
"ja327:8.4": "āvahāmi vahāmi cā”ti.",
"ja327:9.1": "Kākavatījātakaṃ sattamaṃ."
}