{
"ja50:1.1": "Jātaka",
"ja50:2.1": "Ekakanipāta",
"ja50:3.1": "Atthakāmavagga",
"ja50:4.1": "10. Dummedhajātaka",
"ja50:5.1": "“Dummedhānaṃ sahassena,",
"ja50:5.2": "yañño me upayācito;",
"ja50:5.3": "Idāni khohaṃ yajissāmi,",
"ja50:5.4": "bahu adhammiko jano”ti.",
"ja50:6.1": "Dummedhajātakaṃ dasamaṃ.",
"ja50:6.2": "Atthakāmavaggo pañcamo.",
"ja50:7.1": "Tassuddānaṃ",
"ja50:8.1": "Atha mittaka mātu kapotavaro,",
"ja50:8.2": "Tathā veḷuka eḷamūgo rohiṇī;",
"ja50:8.3": "Kapi vāruṇi cetacarā ca puna,",
"ja50:8.4": "Tathā tāraka yaññavarena dasāti.",
"ja50:9.1": "Paṭhamo paṇṇāsako."
}