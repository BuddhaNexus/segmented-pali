{
"ja40:1.1": "Jātaka",
"ja40:2.1": "Ekakanipāta",
"ja40:3.1": "Kulāvakavagga",
"ja40:4.1": "10. Khadiraṅgārajātaka",
"ja40:5.1": "“Kāmaṃ patāmi nirayaṃ,",
"ja40:5.2": "uddhampādo avaṃsiro;",
"ja40:5.3": "Nānariyaṃ karissāmi,",
"ja40:5.4": "handa piṇḍaṃ paṭiggahā”ti.",
"ja40:6.1": "Khadiraṅgārajātakaṃ dasamaṃ.",
"ja40:6.2": "Kulāvakavaggo catuttho.",
"ja40:7.1": "Tassuddānaṃ",
"ja40:8.1": "Sirimātali dhītara pakkhivaro,",
"ja40:8.2": "Ratiyāgato mātāpitā ca puna;",
"ja40:8.3": "Jagatīruha vuḍḍha sukakkaṭako,",
"ja40:8.4": "Tathā nandakapiṇḍavarena dasāti."
}