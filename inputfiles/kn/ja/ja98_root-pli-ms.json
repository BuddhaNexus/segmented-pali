{
  "ja98:0.1": "Jātaka ",
  "ja98:0.2": "Ekakanipāta ",
  "ja98:0.3": "Littavagga ",
  "ja98:0.4": "8. Kūṭavāṇijajātaka ",
  "ja98:1.1": "“Sādhu kho paṇḍito nāma, ",
  "ja98:1.2": "na tveva atipaṇḍito; ",
  "ja98:1.3": "Atipaṇḍitena puttena, ",
  "ja98:1.4": "manamhi upakūḷitoti. ",
  "ja98:2.1": "Kūṭavāṇijajātakaṁ aṭṭhamaṁ. "
}