{
"ja98:1.1": "Jātaka",
"ja98:2.1": "Ekakanipāta",
"ja98:3.1": "Littavagga",
"ja98:4.1": "8. Kūṭavāṇijajātaka",
"ja98:5.1": "“Sādhu kho paṇḍito nāma,",
"ja98:5.2": "na tveva atipaṇḍito;",
"ja98:5.3": "Atipaṇḍitena puttena,",
"ja98:5.4": "manamhi upakūḷitoti.",
"ja98:6.1": "Kūṭavāṇijajātakaṃ aṭṭhamaṃ."
}