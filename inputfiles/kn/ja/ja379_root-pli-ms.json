{
"ja379:1.1": "Jātaka",
"ja379:2.1": "Chakkanipāta",
"ja379:3.1": "Avāriyavagga",
"ja379:4.1": "4. Nerujātaka",
"ja379:5.1": "“Kākolā kākasaṅghā ca,",
"ja379:5.2": "mayañca patataṃ varā;",
"ja379:5.3": "Sabbeva sadisā homa,",
"ja379:5.4": "imaṃ āgamma pabbataṃ.",
"ja379:6.1": "Idha sīhā ca byagghā ca,",
"ja379:6.2": "siṅgālā ca migādhamā;",
"ja379:6.3": "Sabbeva sadisā honti,",
"ja379:6.4": "ayaṃ ko nāma pabbato”.",
"ja379:7.1": "“Imaṃ nerūti jānanti,",
"ja379:7.2": "manussā pabbatuttamaṃ;",
"ja379:7.3": "Idha vaṇṇena sampannā,",
"ja379:7.4": "vasanti sabbapāṇino”.",
"ja379:8.1": "“Amānanā yattha siyā,",
"ja379:8.2": "santānaṃ vā vimānanā;",
"ja379:8.3": "Hīnasammānanā vāpi,",
"ja379:8.4": "na tattha vasatiṃ vase.",
"ja379:9.1": "Yatthālaso ca dakkho ca,",
"ja379:9.2": "sūro bhīru ca pūjiyā;",
"ja379:9.3": "Na tattha santo vasanti,",
"ja379:9.4": "avisesakare nare.",
"ja379:10.1": "Nāyaṃ neru vibhajati,",
"ja379:10.2": "hīnaukkaṭṭhamajjhime;",
"ja379:10.3": "Avisesakaro neru,",
"ja379:10.4": "handa neruṃ jahāmase”ti.",
"ja379:11.1": "Nerujātakaṃ catutthaṃ."
}