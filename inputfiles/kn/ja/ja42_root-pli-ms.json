{
  "ja42:0.1": "Jātaka ",
  "ja42:0.2": "Ekakanipāta ",
  "ja42:0.3": "Atthakāmavagga ",
  "ja42:0.4": "2. Kapotajātaka ",
  "ja42:1.1": "“Yo atthakāmassa hitānukampino, ",
  "ja42:1.2": "Ovajjamāno na karoti sāsanaṁ; ",
  "ja42:1.3": "Kapotakassa vacanaṁ akatvā, ",
  "ja42:1.4": "Amittahatthatthagatova setī”ti. ",
  "ja42:2.1": "Kapotajātakaṁ dutiyaṁ. "
}