{
"ja42:1.1": "Jātaka",
"ja42:2.1": "Ekakanipāta",
"ja42:3.1": "Atthakāmavagga",
"ja42:4.1": "2. Kapotajātaka",
"ja42:5.1": "“Yo atthakāmassa hitānukampino,",
"ja42:5.2": "Ovajjamāno na karoti sāsanaṃ;",
"ja42:5.3": "Kapotakassa vacanaṃ akatvā,",
"ja42:5.4": "Amittahatthatthagatova setī”ti.",
"ja42:6.1": "Kapotajātakaṃ dutiyaṃ."
}