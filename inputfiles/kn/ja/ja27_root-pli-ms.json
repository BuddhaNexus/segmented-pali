{
"ja27:1.1": "Jātaka",
"ja27:2.1": "Ekakanipāta",
"ja27:3.1": "Kuruṅgavagga",
"ja27:4.1": "7. Abhiṇhajātaka",
"ja27:5.1": "“Nālaṃ kabaḷaṃ padātave,",
"ja27:5.2": "Na ca piṇḍaṃ na kuse na ghaṃsituṃ;",
"ja27:5.3": "Maññāmi abhiṇhadassanā,",
"ja27:5.4": "Nāgo snehamakāsi kukkure”ti.",
"ja27:6.1": "Abhiṇhajātakaṃ sattamaṃ."
}