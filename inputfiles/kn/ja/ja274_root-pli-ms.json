{
  "ja274:0.1": "Jātaka ",
  "ja274:0.2": "Tikanipāta ",
  "ja274:0.3": "Udapānavagga ",
  "ja274:0.4": "4. Lolajātaka ",
  "ja274:1.1": "“Kāyaṁ balākā sikhinī, ",
  "ja274:1.2": "corī laṅghipitāmahā; ",
  "ja274:1.3": "Oraṁ balāke āgaccha, ",
  "ja274:1.4": "caṇḍo me vāyaso sakhā”. ",
  "ja274:2.1": "“Nāhaṁ balākā sikhinī, ",
  "ja274:2.2": "ahaṁ lolosmi vāyaso; ",
  "ja274:2.3": "Akatvā vacanaṁ tuyhaṁ, ",
  "ja274:2.4": "passa lūnosmi āgato”. ",
  "ja274:3.1": "“Punapāpajjasī samma, ",
  "ja274:3.2": "sīlañhi tava tādisaṁ; ",
  "ja274:3.3": "Na hi mānusakā bhogā, ",
  "ja274:3.4": "subhuñjā honti pakkhinā”ti. ",
  "ja274:4.1": "Lolajātakaṁ catutthaṁ. "
}