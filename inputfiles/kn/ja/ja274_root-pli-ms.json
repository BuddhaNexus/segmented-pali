{
"ja274:1.1": "Jātaka",
"ja274:2.1": "Tikanipāta",
"ja274:3.1": "Udapānavagga",
"ja274:4.1": "4. Lolajātaka",
"ja274:5.1": "“Kāyaṃ balākā sikhinī,",
"ja274:5.2": "corī laṅghipitāmahā;",
"ja274:5.3": "Oraṃ balāke āgaccha,",
"ja274:5.4": "caṇḍo me vāyaso sakhā”.",
"ja274:6.1": "“Nāhaṃ balākā sikhinī,",
"ja274:6.2": "ahaṃ lolosmi vāyaso;",
"ja274:6.3": "Akatvā vacanaṃ tuyhaṃ,",
"ja274:6.4": "passa lūnosmi āgato”.",
"ja274:7.1": "“Punapāpajjasī samma,",
"ja274:7.2": "sīlañhi tava tādisaṃ;",
"ja274:7.3": "Na hi mānusakā bhogā,",
"ja274:7.4": "subhuñjā honti pakkhinā”ti.",
"ja274:8.1": "Lolajātakaṃ catutthaṃ."
}