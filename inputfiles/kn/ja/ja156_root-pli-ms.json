{
"ja156:1.1": "Jātaka",
"ja156:2.1": "Dukanipāta",
"ja156:3.1": "Daḷhavagga",
"ja156:4.1": "6. Alīnacittajātaka",
"ja156:5.1": "“Alīnacittaṃ nissāya,",
"ja156:5.2": "pahaṭṭhā mahatī camū;",
"ja156:5.3": "Kosalaṃ senāsantuṭṭhaṃ,",
"ja156:5.4": "jīvaggāhaṃ agāhayi.",
"ja156:6.1": "Evaṃ nissayasampanno,",
"ja156:6.2": "bhikkhu āraddhavīriyo;",
"ja156:6.3": "Bhāvayaṃ kusalaṃ dhammaṃ,",
"ja156:6.4": "yogakkhemassa pattiyā;",
"ja156:6.5": "Pāpuṇe anupubbena,",
"ja156:6.6": "sabbasaṃyojanakkhayan”ti.",
"ja156:7.1": "Alīnacittajātakaṃ chaṭṭhaṃ."
}