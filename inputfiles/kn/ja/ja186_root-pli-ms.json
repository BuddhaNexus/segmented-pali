{
"ja186:1.1": "Jātaka",
"ja186:2.1": "Dukanipāta",
"ja186:3.1": "Asadisavagga",
"ja186:4.1": "6. Dadhivāhanajātaka",
"ja186:5.1": "“Vaṇṇagandharasūpeto,",
"ja186:5.2": "amboyaṃ ahuvā pure;",
"ja186:5.3": "Tameva pūjaṃ labhamāno,",
"ja186:5.4": "kenambo kaṭukapphalo”.",
"ja186:6.1": "“Pucimandaparivāro,",
"ja186:6.2": "ambo te dadhivāhana;",
"ja186:6.3": "Mūlaṃ mūlena saṃsaṭṭhaṃ,",
"ja186:6.4": "sākhā sākhā nisevare;",
"ja186:6.5": "Asātasannivāsena,",
"ja186:6.6": "tenambo kaṭukapphalo”ti.",
"ja186:7.1": "Dadhivāhanajātakaṃ chaṭṭhaṃ."
}