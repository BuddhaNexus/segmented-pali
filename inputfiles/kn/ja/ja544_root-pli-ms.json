{
  "ja544:0.1": "Jātaka ",
  "ja544:0.2": "Mahānipāta ",
  "ja544:0.3": "Mūgapakkhavagga ",
  "ja544:0.4": "7 Candakumārajātaka ",
  "ja544:1.1": "Rājāsi luddakammo, ",
  "ja544:1.2": "Ekarājā pupphavatīyā; ",
  "ja544:1.3": "So pucchi brahmabandhuṁ, ",
  "ja544:1.4": "Khaṇḍahālaṁ purohitaṁ mūḷhaṁ. ",
  "ja544:2.1": "“Saggāna maggamācikkha, ",
  "ja544:2.2": "Tvaṁsi brāhmaṇa dhammavinayakusalo; ",
  "ja544:2.3": "Yathā ito vajanti sugatiṁ, ",
  "ja544:2.4": "Narā puññāni katvāna”. ",
  "ja544:3.1": "“Atidānaṁ daditvāna, ",
  "ja544:3.2": "avajjhe deva ghātetvā; ",
  "ja544:3.3": "Evaṁ vajanti sugatiṁ, ",
  "ja544:3.4": "narā puññāni katvāna”. ",
  "ja544:4.1": "“Kiṁ pana taṁ atidānaṁ, ",
  "ja544:4.2": "Ke ca avajjhā imasmi lokasmiṁ; ",
  "ja544:4.3": "Etañca kho no akkhāhi, ",
  "ja544:4.4": "Yajissāmi dadāmi dānāni”. ",
  "ja544:5.1": "“Puttehi deva yajitabbaṁ, ",
  "ja544:5.2": "Mahesīhi negamehi ca; ",
  "ja544:5.3": "Usabhehi ājāniyehi catūhi, ",
  "ja544:5.4": "Sabbacatukkena deva yajitabbaṁ”. ",
  "ja544:6.1": "Taṁ sutvā antepure, ",
  "ja544:6.2": "Kumārā mahesiyo ca haññantu; ",
  "ja544:6.3": "Eko ahosi nigghoso, ",
  "ja544:6.4": "Bhismā accuggato saddo. ",
  "ja544:7.1": "“Gacchatha vadetha kumāre, ",
  "ja544:7.2": "Candaṁ sūriyañca bhaddasenañca; ",
  "ja544:7.3": "Sūrañca vāmagottañca, ",
  "ja544:7.4": "Pacurā kira hotha yaññatthāya. ",
  "ja544:8.1": "Kumāriyopi vadetha, ",
  "ja544:8.2": "Upasenaṁ kokilañca muditañca; ",
  "ja544:8.3": "Nandañcāpi kumāriṁ, ",
  "ja544:8.4": "Pacurā kira hotha yaññatthāya. ",
  "ja544:9.1": "Vijayampi mayhaṁ mahesiṁ, ",
  "ja544:9.2": "Erāvatiṁ kesiniṁ sunandañca; ",
  "ja544:9.3": "Lakkhaṇavarūpapannā, ",
  "ja544:9.4": "Pacurā kira hotha yaññatthāya. ",
  "ja544:10.1": "Gahapatayo ca vadetha, ",
  "ja544:10.2": "Puṇṇamukhaṁ bhaddiyaṁ siṅgālañca; ",
  "ja544:10.3": "Vaḍḍhañcāpi gahapatiṁ, ",
  "ja544:10.4": "Pacurā kira hotha yaññatthāya”. ",
  "ja544:11.1": "Te tattha gahapatayo, ",
  "ja544:11.2": "Avocisuṁ samāgatā puttadāraparikiṇṇā; ",
  "ja544:11.3": "“Sabbeva sikhino deva karohi, ",
  "ja544:11.4": "Atha vā no dāse sāvehi”. ",
  "ja544:12.1": "“Abhayaṅkarampi me hatthiṁ, ",
  "ja544:12.2": "Nāḷāgiriṁ accuggataṁ varuṇadantaṁ; ",
  "ja544:12.3": "Ānetha kho ne khippaṁ, ",
  "ja544:12.4": "Yaññatthāya bhavissanti. ",
  "ja544:13.1": "Assaratanampi kesiṁ, ",
  "ja544:13.2": "Surāmukhaṁ puṇṇakaṁ vinatakañca; ",
  "ja544:13.3": "Ānetha kho ne khippaṁ, ",
  "ja544:13.4": "Yaññatthāya bhavissanti. ",
  "ja544:14.1": "Usabhampi yūthapatiṁ anojaṁ, ",
  "ja544:14.2": "Nisabhaṁ gavampatiṁ tepi mayhaṁ ānetha; ",
  "ja544:14.3": "Samūha karontu sabbaṁ, ",
  "ja544:14.4": "Yajissāmi dadāmi dānāni. ",
  "ja544:15.1": "Sabbaṁ paṭiyādetha, ",
  "ja544:15.2": "Yaññaṁ pana uggatamhi sūriyamhi; ",
  "ja544:15.3": "Āṇāpetha ca kumāre, ",
  "ja544:15.4": "Abhiramantu imaṁ rattiṁ. ",
  "ja544:16.1": "Sabbaṁ upaṭṭhapetha, ",
  "ja544:16.2": "Yaññaṁ pana uggatamhi sūriyamhi; ",
  "ja544:16.3": "Vadetha dāni kumāre, ",
  "ja544:16.4": "Ajja kho pacchimā ratti”. ",
  "ja544:17.1": "Taṁtaṁ mātā avaca, ",
  "ja544:17.2": "Rodantī āgantvā vimānato; ",
  "ja544:17.3": "“Yañño kira te putta, ",
  "ja544:17.4": "Bhavissati catūhi puttehi”. ",
  "ja544:18.1": "“Sabbepi mayhaṁ puttā cattā, ",
  "ja544:18.2": "candasmiṁ haññamānasmiṁ; ",
  "ja544:18.3": "Puttehi yaññaṁ yajitvāna, ",
  "ja544:18.4": "sugatiṁ saggaṁ gamissāmi”. ",
  "ja544:19.1": "“Mā taṁ putta saddahesi, ",
  "ja544:19.2": "Sugati kira hoti puttayaññena; ",
  "ja544:19.3": "Nirayāneso maggo, ",
  "ja544:19.4": "Neso maggo hi saggānaṁ. ",
  "ja544:20.1": "Dānāni dehi koṇḍañña, ",
  "ja544:20.2": "Ahiṁsā sabbabhūtabhabyānaṁ; ",
  "ja544:20.3": "Esa maggo sugatiyā, ",
  "ja544:20.4": "Na ca maggo puttayaññena”. ",
  "ja544:21.1": "“Ācariyānaṁ vacanā, ",
  "ja544:21.2": "Ghātessaṁ candañca sūriyañca; ",
  "ja544:21.3": "Puttehi yaññaṁ yajitvāna duccajehi, ",
  "ja544:21.4": "Sugatiṁ saggaṁ gamissāmi”. ",
  "ja544:22.1": "Taṁtaṁ pitāpi avaca, ",
  "ja544:22.2": "Vasavattī orasaṁ sakaṁ puttaṁ; ",
  "ja544:22.3": "“Yañño kira te putta, ",
  "ja544:22.4": "Bhavissati catūhi puttehi”. ",
  "ja544:23.1": "“Sabbepi mayhaṁ puttā cattā, ",
  "ja544:23.2": "candasmiṁ haññamānasmiṁ; ",
  "ja544:23.3": "Puttehi yaññaṁ yajitvāna, ",
  "ja544:23.4": "sugatiṁ saggaṁ gamissāmi”. ",
  "ja544:24.1": "“Mā taṁ putta saddahesi, ",
  "ja544:24.2": "Sugati kira hoti puttayaññena; ",
  "ja544:24.3": "Nirayāneso maggo, ",
  "ja544:24.4": "Neso maggo hi saggānaṁ. ",
  "ja544:25.1": "Dānāni dehi koṇḍañña, ",
  "ja544:25.2": "Ahiṁsā sabbabhūtabhabyānaṁ; ",
  "ja544:25.3": "Esa maggo sugatiyā, ",
  "ja544:25.4": "Na ca maggo puttayaññena”. ",
  "ja544:26.1": "“Ācariyānaṁ vacanā, ",
  "ja544:26.2": "Ghātessaṁ candañca sūriyañca; ",
  "ja544:26.3": "Puttehi yaññaṁ yajitvāna duccajehi, ",
  "ja544:26.4": "Sugatiṁ saggaṁ gamissāmi”. ",
  "ja544:27.1": "“Dānāni dehi koṇḍañña, ",
  "ja544:27.2": "Ahiṁsā sabbabhūtabhabyānaṁ; ",
  "ja544:27.3": "Puttaparivuto tuvaṁ, ",
  "ja544:27.4": "Raṭṭhaṁ janapadañca pālehi”. ",
  "ja544:28.1": "“Mā no deva avadhi, ",
  "ja544:28.2": "Dāse no dehi khaṇḍahālassa; ",
  "ja544:28.3": "Api nigaḷabandhakāpi, ",
  "ja544:28.4": "Hatthī asse ca pālema. ",
  "ja544:29.1": "Mā no deva avadhi, ",
  "ja544:29.2": "Dāse no dehi khaṇḍahālassa; ",
  "ja544:29.3": "Api nigaḷabandhakāpi, ",
  "ja544:29.4": "Hatthichakaṇāni ujjhema. ",
  "ja544:30.1": "Mā no deva avadhi, ",
  "ja544:30.2": "Dāse no dehi khaṇḍahālassa; ",
  "ja544:30.3": "Api nigaḷabandhakāpi, ",
  "ja544:30.4": "Assachakaṇāni ujjhema. ",
  "ja544:31.1": "Mā no deva avadhi, ",
  "ja544:31.2": "Dāse no dehi khaṇḍahālassa; ",
  "ja544:31.3": "Yassa honti tava kāmā, ",
  "ja544:31.4": "Api raṭṭhā pabbājitā; ",
  "ja544:31.5": "Bhikkhācariyaṁ carissāma”. ",
  "ja544:32.1": "“Dukkhaṁ kho me janayatha, ",
  "ja544:32.2": "Vilapantā jīvitassa kāmā hi; ",
  "ja544:32.3": "Muñcetha dāni kumāre, ",
  "ja544:32.4": "Alampi me hotu puttayaññena”. ",
  "ja544:33.1": "“Pubbeva khosi me vutto, ",
  "ja544:33.2": "Dukkaraṁ durabhisambhavañcetaṁ; ",
  "ja544:33.3": "Atha no upakkhaṭassa yaññassa, ",
  "ja544:33.4": "Kasmā karosi vikkhepaṁ. ",
  "ja544:34.1": "Sabbe vajanti sugatiṁ, ",
  "ja544:34.2": "Ye yajanti yepi yājenti; ",
  "ja544:34.3": "Ye cāpi anumodanti, ",
  "ja544:34.4": "Yajantānaṁ edisaṁ mahāyaññaṁ”. ",
  "ja544:35.1": "“Atha kissa jano pubbe, ",
  "ja544:35.2": "Sotthānaṁ brāhmaṇe avācesi; ",
  "ja544:35.3": "Atha no akāraṇasmā, ",
  "ja544:35.4": "Yaññatthāya deva ghātesi. ",
  "ja544:36.1": "Pubbeva no daharakāle, ",
  "ja544:36.2": "na hanesi na ghātesi; ",
  "ja544:36.3": "Daharamhā yobbanaṁ pattā, ",
  "ja544:36.4": "adūsakā tāta haññāma. ",
  "ja544:37.1": "Hatthigate assagate, ",
  "ja544:37.2": "Sannaddhe passa no mahārāja; ",
  "ja544:37.3": "Yuddhe vā yujjhamāne vā, ",
  "ja544:37.4": "Na hi mādisā sūrā honti yaññatthāya. ",
  "ja544:38.1": "Paccante vāpi kupite, ",
  "ja544:38.2": "Aṭavīsu vā mādise niyojenti; ",
  "ja544:38.3": "Atha no akāraṇasmā, ",
  "ja544:38.4": "Abhūmiyaṁ tāta haññāma. ",
  "ja544:39.1": "Yāpi hi tā sakuṇiyo, ",
  "ja544:39.2": "Vasanti tiṇagharāni katvāna; ",
  "ja544:39.3": "Tāsampi piyā puttā, ",
  "ja544:39.4": "Atha no tvaṁ deva ghātesi. ",
  "ja544:40.1": "Mā tassa saddahesi, ",
  "ja544:40.2": "Na maṁ khaṇḍahālo ghāteyya; ",
  "ja544:40.3": "Mamañhi so ghātetvāna, ",
  "ja544:40.4": "Anantarā tampi deva ghāteyya. ",
  "ja544:41.1": "Gāmavaraṁ nigamavaraṁ dadanti, ",
  "ja544:41.2": "Bhogampissa mahārāja; ",
  "ja544:41.3": "Athaggapiṇḍikāpi, ",
  "ja544:41.4": "Kule kule hete bhuñjanti. ",
  "ja544:42.1": "Tesampi tādisānaṁ, ",
  "ja544:42.2": "Icchanti dubbhituṁ mahārāja; ",
  "ja544:42.3": "Yebhuyyena ete, ",
  "ja544:42.4": "Akataññuno brāhmaṇā deva. ",
  "ja544:43.1": "Mā no deva avadhi, ",
  "ja544:43.2": "Dāse no dehi khaṇḍahālassa; ",
  "ja544:43.3": "Api nigaḷabandhakāpi, ",
  "ja544:43.4": "Hatthī asse ca pālema. ",
  "ja544:44.1": "Mā no deva avadhi, ",
  "ja544:44.2": "Dāse no dehi khaṇḍahālassa; ",
  "ja544:44.3": "Api nigaḷabandhakāpi, ",
  "ja544:44.4": "Hatthichakaṇāni ujjhema. ",
  "ja544:45.1": "Mā no deva avadhi, ",
  "ja544:45.2": "Dāse no dehi khaṇḍahālassa; ",
  "ja544:45.3": "Api nigaḷabandhakāpi, ",
  "ja544:45.4": "Assachakaṇāni ujjhema. ",
  "ja544:46.1": "Mā no deva avadhi, ",
  "ja544:46.2": "Dāse no dehi khaṇḍahālassa; ",
  "ja544:46.3": "Yassa honti tava kāmā, ",
  "ja544:46.4": "Api raṭṭhā pabbājitā; ",
  "ja544:46.5": "Bhikkhācariyaṁ carissāma”. ",
  "ja544:47.1": "“Dukkhaṁ kho me janayatha, ",
  "ja544:47.2": "Vilapantā jīvitassa kāmā hi; ",
  "ja544:47.3": "Muñcetha dāni kumāre, ",
  "ja544:47.4": "Alampi me hotu puttayaññena”. ",
  "ja544:48.1": "“Pubbeva khosi me vutto, ",
  "ja544:48.2": "Dukkaraṁ durabhisambhavañcetaṁ; ",
  "ja544:48.3": "Atha no upakkhaṭassa yaññassa, ",
  "ja544:48.4": "Kasmā karosi vikkhepaṁ. ",
  "ja544:49.1": "Sabbe vajanti sugatiṁ, ",
  "ja544:49.2": "Ye yajanti yepi yājenti; ",
  "ja544:49.3": "Ye cāpi anumodanti, ",
  "ja544:49.4": "Yajantānaṁ edisaṁ mahāyaññaṁ”. ",
  "ja544:50.1": "“Yadi kira yajitvā puttehi, ",
  "ja544:50.2": "Devalokaṁ ito cutā yanti; ",
  "ja544:50.3": "Brāhmaṇo tāva yajatu, ",
  "ja544:50.4": "Pacchāpi yajasi tuvaṁ rājā. ",
  "ja544:51.1": "Yadi kira yajitvā puttehi, ",
  "ja544:51.2": "Devalokaṁ ito cutā yanti; ",
  "ja544:51.3": "Esveva khaṇḍahālo, ",
  "ja544:51.4": "Yajataṁ sakehi puttehi. ",
  "ja544:52.1": "Evaṁ jānanto khaṇḍahālo, ",
  "ja544:52.2": "kiṁ puttake na ghātesi; ",
  "ja544:52.3": "Sabbañca ñātijanaṁ, ",
  "ja544:52.4": "attānañca na ghātesi. ",
  "ja544:53.1": "Sabbe vajanti nirayaṁ, ",
  "ja544:53.2": "Ye yajanti yepi yājenti; ",
  "ja544:53.3": "Ye cāpi anumodanti, ",
  "ja544:53.4": "Yajantānaṁ edisaṁ mahāyaññaṁ. ",
  "ja544:54.1": "Sace hi so sujjhati yo hanāti, ",
  "ja544:54.2": "Hatopi so saggamupeti ṭhānaṁ; ",
  "ja544:54.3": "Bhovādi bhovādina mārayeyyuṁ, ",
  "ja544:54.4": "Ye cāpi tesaṁ abhisaddaheyyuṁ. ",
  "ja544:55.1": "Kathañca kira puttakāmāyo, ",
  "ja544:55.2": "Gahapatayo gharaṇiyo ca; ",
  "ja544:55.3": "Nagaramhi na uparavanti rājānaṁ, ",
  "ja544:55.4": "‘Mā ghātayi orasaṁ puttaṁ’. ",
  "ja544:56.1": "Kathañca kira puttakāmāyo, ",
  "ja544:56.2": "Gahapatayo gharaṇiyo ca; ",
  "ja544:56.3": "Nagaramhi na uparavanti rājānaṁ, ",
  "ja544:56.4": "‘Mā ghātayi atrajaṁ puttaṁ’. ",
  "ja544:57.1": "Rañño camhi atthakāmo, ",
  "ja544:57.2": "Hito ca sabbajanapadassa; ",
  "ja544:57.3": "Na koci assa paṭighaṁ, ",
  "ja544:57.4": "Mayā jānapado na pavedeti. ",
  "ja544:58.1": "Gacchatha vo gharaṇiyo, ",
  "ja544:58.2": "Tātañca vadetha khaṇḍahālañca; ",
  "ja544:58.3": "‘Mā ghātetha kumāre, ",
  "ja544:58.4": "Adūsake sīhasaṅkāse’. ",
  "ja544:59.1": "Gacchatha vo gharaṇiyo, ",
  "ja544:59.2": "Tātañca vadetha khaṇḍahālañca; ",
  "ja544:59.3": "‘Mā ghātetha kumāre, ",
  "ja544:59.4": "Apekkhite sabbalokassa’. ",
  "ja544:60.1": "Yannūnāhaṁ jāyeyyaṁ, ",
  "ja544:60.2": "Rathakārakulesu vā; ",
  "ja544:60.3": "Pukkusakulesu vā vessesu vā jāyeyyaṁ, ",
  "ja544:60.4": "Na hajja maṁ rāja yaññe ghāteyya”. ",
  "ja544:61.1": "“Sabbā sīmantiniyo gacchatha, ",
  "ja544:61.2": "Ayyassa khaṇḍahālassa; ",
  "ja544:61.3": "Pādesu nipatatha, ",
  "ja544:61.4": "Aparādhāhaṁ na passāmi. ",
  "ja544:62.1": "Sabbā sīmantiniyo gacchatha, ",
  "ja544:62.2": "Ayyassa khaṇḍahālassa; ",
  "ja544:62.3": "Pādesu nipatatha, ",
  "ja544:62.4": "Kinte bhante mayaṁ adūsema”. ",
  "ja544:63.1": "Kapaṇā vilapati selā, ",
  "ja544:63.2": "Disvāna bhātare upanītatte; ",
  "ja544:63.3": "“Yañño kira me ukkhipito, ",
  "ja544:63.4": "Tātena saggakāmena”. ",
  "ja544:64.1": "Āvatti parivatti ca, ",
  "ja544:64.2": "vasulo sammukhā rañño; ",
  "ja544:64.3": "“Mā no pitaraṁ avadhi, ",
  "ja544:64.4": "daharamhāyobbanaṁ pattā”. ",
  "ja544:65.1": "“Eso te vasula pitā, ",
  "ja544:65.2": "Samehi pitarā saha; ",
  "ja544:65.3": "Dukkhaṁ kho me janayasi, ",
  "ja544:65.4": "Vilapanto antepurasmiṁ; ",
  "ja544:65.5": "Muñcetha dāni kumāre, ",
  "ja544:65.6": "Alampi me hotu puttayaññena”. ",
  "ja544:66.1": "“Pubbeva khosi me vutto, ",
  "ja544:66.2": "Dukkaraṁ durabhisambhavañcetaṁ; ",
  "ja544:66.3": "Atha no upakkhaṭassa yaññassa, ",
  "ja544:66.4": "Kasmā karosi vikkhepaṁ. ",
  "ja544:67.1": "Sabbe vajanti sugatiṁ, ",
  "ja544:67.2": "Ye yajanti yepi yājenti; ",
  "ja544:67.3": "Ye cāpi anumodanti, ",
  "ja544:67.4": "Yajantānaṁ edisaṁ mahāyaññaṁ. ",
  "ja544:68.1": "Sabbaratanassa yañño upakkhaṭo, ",
  "ja544:68.2": "Ekarāja tava paṭiyatto; ",
  "ja544:68.3": "Abhinikkhamassu deva, ",
  "ja544:68.4": "Saggaṁ gato tvaṁ pamodissasi”. ",
  "ja544:69.1": "Daharā sattasatā etā, ",
  "ja544:69.2": "Candakumārassa bhariyāyo; ",
  "ja544:69.3": "Kese pakiritvāna, ",
  "ja544:69.4": "Rodantiyo maggamanuyāyisuṁ. ",
  "ja544:70.1": "Aparā pana sokena, ",
  "ja544:70.2": "Nikkhantā nandane viya devā; ",
  "ja544:70.3": "Kese pakiritvāna, ",
  "ja544:70.4": "Rodantiyo maggamanuyāyiṁsu. ",
  "ja544:71.1": "“Kāsikasucivatthadharā, ",
  "ja544:71.2": "Kuṇḍalino agalucandanavilittā; ",
  "ja544:71.3": "Niyyanti candasūriyā, ",
  "ja544:71.4": "Yaññatthāya ekarājassa. ",
  "ja544:72.1": "Kāsikasucivatthadharā, ",
  "ja544:72.2": "Kuṇḍalino agalucandanavilittā; ",
  "ja544:72.3": "Niyyanti candasūriyā, ",
  "ja544:72.4": "Mātu katvā hadayasokaṁ. ",
  "ja544:73.1": "Kāsikasucivatthadharā, ",
  "ja544:73.2": "Kuṇḍalino agalucandanavilittā; ",
  "ja544:73.3": "Niyyanti candasūriyā, ",
  "ja544:73.4": "Janassa katvā hadayasokaṁ. ",
  "ja544:74.1": "Maṁsarasabhojanā nhāpakasunhāpitā, ",
  "ja544:74.2": "Kuṇḍalino agalucandanavilittā; ",
  "ja544:74.3": "Niyyanti candasūriyā, ",
  "ja544:74.4": "Yaññatthāya ekarājassa. ",
  "ja544:75.1": "Maṁsarasabhojanā nhāpakasunhāpitā, ",
  "ja544:75.2": "Kuṇḍalino agalucandanavilittā; ",
  "ja544:75.3": "Niyyanti candasūriyā, ",
  "ja544:75.4": "Mātu katvā hadayasokaṁ. ",
  "ja544:76.1": "Maṁsarasabhojanā nhāpakasunhāpitā, ",
  "ja544:76.2": "Kuṇḍalino agalucandanavilittā; ",
  "ja544:76.3": "Niyyanti candasūriyā, ",
  "ja544:76.4": "Janassa katvā hadayasokaṁ. ",
  "ja544:77.1": "Yassu pubbe hatthivaradhuragate, ",
  "ja544:77.2": "Hatthīhi anuvajanti; ",
  "ja544:77.3": "Tyajja candasūriyā, ",
  "ja544:77.4": "Ubhova pattikā yanti. ",
  "ja544:78.1": "Yassu pubbe assavaradhuragate, ",
  "ja544:78.2": "Assehi anuvajanti; ",
  "ja544:78.3": "Tyajja candasūriyā, ",
  "ja544:78.4": "Ubhova pattikā yanti. ",
  "ja544:79.1": "Yassu pubbe rathavaradhuragate, ",
  "ja544:79.2": "Rathehi anuvajanti; ",
  "ja544:79.3": "Tyajja candasūriyā, ",
  "ja544:79.4": "Ubhova pattikā yanti. ",
  "ja544:80.1": "Yehissu pubbe niyyaṁsu, ",
  "ja544:80.2": "Tapanīyakappanehi turaṅgehi; ",
  "ja544:80.3": "Tyajja candasūriyā, ",
  "ja544:80.4": "Ubhova pattikā yanti”. ",
  "ja544:81.1": "“Yadi sakuṇi maṁsamicchasi, ",
  "ja544:81.2": "Ḍayassu pubbena pupphavatiyā; ",
  "ja544:81.3": "Yajatettha ekarājā, ",
  "ja544:81.4": "Sammūḷho catūhi puttehi. ",
  "ja544:82.1": "Yadi sakuṇi maṁsamicchasi, ",
  "ja544:82.2": "Ḍayassu pubbena pupphavatiyā; ",
  "ja544:82.3": "Yajatettha ekarājā, ",
  "ja544:82.4": "Sammūḷho catūhi kaññāhi. ",
  "ja544:83.1": "Yadi sakuṇi maṁsamicchasi, ",
  "ja544:83.2": "Ḍayassu pubbena pupphavatiyā; ",
  "ja544:83.3": "Yajatettha ekarājā, ",
  "ja544:83.4": "Sammūḷho catūhi mahesīhi. ",
  "ja544:84.1": "Yadi sakuṇi maṁsamicchasi, ",
  "ja544:84.2": "Ḍayassu pubbena pupphavatiyā; ",
  "ja544:84.3": "Yajatettha ekarājā, ",
  "ja544:84.4": "Sammūḷho catūhi gahapatīhi. ",
  "ja544:85.1": "Yadi sakuṇi maṁsamicchasi, ",
  "ja544:85.2": "Ḍayassu pubbena pupphavatiyā; ",
  "ja544:85.3": "Yajatettha ekarājā, ",
  "ja544:85.4": "Sammūḷho catūhi hatthīhi. ",
  "ja544:86.1": "Yadi sakuṇi maṁsamicchasi, ",
  "ja544:86.2": "Ḍayassu pubbena pupphavatiyā; ",
  "ja544:86.3": "Yajatettha ekarājā, ",
  "ja544:86.4": "Sammūḷho catūhi assehi. ",
  "ja544:87.1": "Yadi sakuṇi maṁsamicchasi, ",
  "ja544:87.2": "Ḍayassu pubbena pupphavatiyā; ",
  "ja544:87.3": "Yajatettha ekarājā, ",
  "ja544:87.4": "Sammūḷho catūhi usabhehi. ",
  "ja544:88.1": "Yadi sakuṇi maṁsamicchasi, ",
  "ja544:88.2": "Ḍayassu pubbena pupphavatiyā; ",
  "ja544:88.3": "Yajatettha ekarājā, ",
  "ja544:88.4": "Sammūḷho sabbacatukkena. ",
  "ja544:89.1": "Ayamassa pāsādo, ",
  "ja544:89.2": "Idaṁ antepuraṁ suramaṇīyaṁ; ",
  "ja544:89.3": "Tedāni ayyaputtā, ",
  "ja544:89.4": "Cattāro vadhāya ninnītā. ",
  "ja544:90.1": "Idamassa kūṭāgāraṁ, ",
  "ja544:90.2": "Sovaṇṇaṁ pupphamalyavikiṇṇaṁ; ",
  "ja544:90.3": "Tedāni ayyaputtā, ",
  "ja544:90.4": "Cattāro vadhāya ninnītā. ",
  "ja544:91.1": "Idamassa uyyānaṁ, ",
  "ja544:91.2": "Supupphitaṁ sabbakālikaṁ rammaṁ; ",
  "ja544:91.3": "Tedāni ayyaputtā, ",
  "ja544:91.4": "Cattāro vadhāya ninnītā. ",
  "ja544:92.1": "Idamassa asokavanaṁ, ",
  "ja544:92.2": "Supupphitaṁ sabbakālikaṁ rammaṁ; ",
  "ja544:92.3": "Tedāni ayyaputtā, ",
  "ja544:92.4": "Cattāro vadhāya ninnītā. ",
  "ja544:93.1": "Idamassa kaṇikāravanaṁ, ",
  "ja544:93.2": "Supupphitaṁ sabbakālikaṁ rammaṁ; ",
  "ja544:93.3": "Tedāni ayyaputtā, ",
  "ja544:93.4": "Cattāro vadhāya ninnītā. ",
  "ja544:94.1": "Idamassa pāṭalivanaṁ, ",
  "ja544:94.2": "Supupphitaṁ sabbakālikaṁ rammaṁ; ",
  "ja544:94.3": "Tedāni ayyaputtā, ",
  "ja544:94.4": "Cattāro vadhāya ninnītā. ",
  "ja544:95.1": "Idamassa ambavanaṁ, ",
  "ja544:95.2": "Supupphitaṁ sabbakālikaṁ rammaṁ; ",
  "ja544:95.3": "Tedāni ayyaputtā, ",
  "ja544:95.4": "Cattāro vadhāya ninnītā. ",
  "ja544:96.1": "Ayamassa pokkharaṇī, ",
  "ja544:96.2": "Sañchannā padumapuṇḍarīkehi; ",
  "ja544:96.3": "Nāvā ca sovaṇṇavikatā, ",
  "ja544:96.4": "Pupphavalliyā cittā suramaṇīyā; ",
  "ja544:96.5": "Tedāni ayyaputtā, ",
  "ja544:96.6": "Cattāro vadhāya ninnītā. ",
  "ja544:97.1": "Idamassa hatthiratanaṁ, ",
  "ja544:97.2": "Erāvaṇo gajo balī dantī; ",
  "ja544:97.3": "Tedāni ayyaputtā, ",
  "ja544:97.4": "Cattāro vadhāya ninnītā. ",
  "ja544:98.1": "Idamassa assaratanaṁ, ",
  "ja544:98.2": "ekakhuro asso; ",
  "ja544:98.3": "Tedāni ayyaputtā, ",
  "ja544:98.4": "cattāro vadhāya ninnītā. ",
  "ja544:99.1": "Ayamassa assaratho, ",
  "ja544:99.2": "Sāḷiya nigghoso subho ratanavicitto; ",
  "ja544:99.3": "Yatthassu ayyaputtā, ",
  "ja544:99.4": "Sobhiṁsu nandane viya devā; ",
  "ja544:99.5": "Tedāni ayyaputtā, ",
  "ja544:99.6": "Cattāro vadhāya ninnītā. ",
  "ja544:100.1": "Kathaṁ nāma sāmasamasundarehi, ",
  "ja544:100.2": "Candanamudukagattehi; ",
  "ja544:100.3": "Rājā yajissate yaññaṁ, ",
  "ja544:100.4": "Sammūḷho catūhi puttehi. ",
  "ja544:101.1": "Kathaṁ nāma sāmasamasundarāhi, ",
  "ja544:101.2": "Candanamudukagattāhi; ",
  "ja544:101.3": "Rājā yajissate yaññaṁ, ",
  "ja544:101.4": "Sammūḷho catūhi kaññāhi. ",
  "ja544:102.1": "Kathaṁ nāma sāmasamasundarāhi, ",
  "ja544:102.2": "Candanamudukagattāhi; ",
  "ja544:102.3": "Rājā yajissate yaññaṁ, ",
  "ja544:102.4": "Sammūḷho catūhi mahesīhi. ",
  "ja544:103.1": "Kathaṁ nāma sāmasamasundarehi, ",
  "ja544:103.2": "Candanamudukagattehi; ",
  "ja544:103.3": "Rājā yajissate yaññaṁ, ",
  "ja544:103.4": "Sammūḷho catūhi gahapatīhi. ",
  "ja544:104.1": "Yathā honti gāmanigamā, ",
  "ja544:104.2": "Suññā amanussakā brahāraññā; ",
  "ja544:104.3": "Tathā hessati pupphavatiyā, ",
  "ja544:104.4": "Yiṭṭhesu candasūriyesu”. ",
  "ja544:105.1": "“Ummattikā bhavissāmi, ",
  "ja544:105.2": "Bhūnahatā paṁsunā ca parikiṇṇā; ",
  "ja544:105.3": "Sace candavaraṁ hanti, ",
  "ja544:105.4": "Pāṇā me deva rujjhanti. ",
  "ja544:106.1": "Ummattikā bhavissāmi, ",
  "ja544:106.2": "Bhūnahatā paṁsunā ca parikiṇṇā; ",
  "ja544:106.3": "Sace sūriyavaraṁ hanti, ",
  "ja544:106.4": "Pāṇā me deva rujjhanti”. ",
  "ja544:107.1": "“Kinnumā na ramāpeyyuṁ, ",
  "ja544:107.2": "Aññamaññaṁ piyaṁvadā; ",
  "ja544:107.3": "Ghaṭṭikā uparikkhī ca, ",
  "ja544:107.4": "Pokkharaṇī ca bhārikā; ",
  "ja544:107.5": "Candasūriyesu naccantiyo, ",
  "ja544:107.6": "Samā tāsaṁ na vijjati”. ",
  "ja544:108.1": "“Imaṁ mayhaṁ hadayasokaṁ, ",
  "ja544:108.2": "Paṭimuñcatu khaṇḍahāla tava mātā; ",
  "ja544:108.3": "Yo mayhaṁ hadayasoko, ",
  "ja544:108.4": "Candamhi vadhāya ninnīte. ",
  "ja544:109.1": "Imaṁ mayhaṁ hadayasokaṁ, ",
  "ja544:109.2": "Paṭimuñcatu khaṇḍahāla tava mātā; ",
  "ja544:109.3": "Yo mayhaṁ hadayasoko, ",
  "ja544:109.4": "Sūriyamhi vadhāya ninnīte. ",
  "ja544:110.1": "Imaṁ mayhaṁ hadayasokaṁ, ",
  "ja544:110.2": "Paṭimuñcatu khaṇḍahāla tava jāyā; ",
  "ja544:110.3": "Yo mayhaṁ hadayasoko, ",
  "ja544:110.4": "Candamhi vadhāya ninnīte. ",
  "ja544:111.1": "Imaṁ mayhaṁ hadayasokaṁ, ",
  "ja544:111.2": "Paṭimuñcatu khaṇḍahāla tava jāyā; ",
  "ja544:111.3": "Yo mayhaṁ hadayasoko, ",
  "ja544:111.4": "Sūriyamhi vadhāya ninnīte. ",
  "ja544:112.1": "Mā ca putte mā ca patiṁ, ",
  "ja544:112.2": "Addakkhi khaṇḍahāla tava mātā; ",
  "ja544:112.3": "Yo ghātesi kumāre, ",
  "ja544:112.4": "Adūsake sīhasaṅkāse. ",
  "ja544:113.1": "Mā ca putte mā ca patiṁ, ",
  "ja544:113.2": "Addakkhi khaṇḍahāla tava mātā; ",
  "ja544:113.3": "Yo ghātesi kumāre, ",
  "ja544:113.4": "Apekkhite sabbalokassa. ",
  "ja544:114.1": "Mā ca putte mā ca patiṁ, ",
  "ja544:114.2": "Addakkhi khaṇḍahāla tava jāyā; ",
  "ja544:114.3": "Yo ghātesi kumāre, ",
  "ja544:114.4": "Adūsake sīhasaṅkāse. ",
  "ja544:115.1": "Mā ca putte mā ca patiṁ, ",
  "ja544:115.2": "Addakkhi khaṇḍahāla tava jāyā; ",
  "ja544:115.3": "Yo ghātesi kumāre, ",
  "ja544:115.4": "Apekkhite sabbalokassa”. ",
  "ja544:116.1": "“Mā no deva avadhi, ",
  "ja544:116.2": "Dāse no dehi khaṇḍahālassa; ",
  "ja544:116.3": "Api nigaḷabandhakāpi, ",
  "ja544:116.4": "Hatthī asse ca pālema. ",
  "ja544:117.1": "Mā no deva avadhi, ",
  "ja544:117.2": "Dāse no dehi khaṇḍahālassa; ",
  "ja544:117.3": "Api nigaḷabandhakāpi, ",
  "ja544:117.4": "Hatthichakaṇāni ujjhema. ",
  "ja544:118.1": "Mā no deva avadhi, ",
  "ja544:118.2": "Dāse no dehi khaṇḍahālassa; ",
  "ja544:118.3": "Api nigaḷabandhakāpi, ",
  "ja544:118.4": "Assachakaṇāni ujjhema. ",
  "ja544:119.1": "Mā no deva avadhi, ",
  "ja544:119.2": "Dāse no dehi khaṇḍahālassa; ",
  "ja544:119.3": "Yassa honti tava kāmā, ",
  "ja544:119.4": "Api raṭṭhā pabbājitā; ",
  "ja544:119.5": "Bhikkhācariyaṁ carissāma. ",
  "ja544:120.1": "Dibbaṁ deva upayācanti, ",
  "ja544:120.2": "puttatthikāpi daliddā; ",
  "ja544:120.3": "Paṭibhānānipi hitvā, ",
  "ja544:120.4": "putte na labhanti ekaccā. ",
  "ja544:121.1": "Āsīsikāni karonti, ",
  "ja544:121.2": "Puttā no jāyantu tato paputtā; ",
  "ja544:121.3": "Atha no akāraṇasmā, ",
  "ja544:121.4": "Yaññatthāya deva ghātesi. ",
  "ja544:122.1": "Upayācitakena puttaṁ labhanti, ",
  "ja544:122.2": "Mā tāta no aghātesi; ",
  "ja544:122.3": "Mā kicchāladdhakehi puttehi, ",
  "ja544:122.4": "Yajittho imaṁ yaññaṁ. ",
  "ja544:123.1": "Upayācitakena puttaṁ labhanti, ",
  "ja544:123.2": "Mā tāta no aghātesi; ",
  "ja544:123.3": "Mā kapaṇaladdhakehi puttehi, ",
  "ja544:123.4": "Ammāya no vippavāsesi. ",
  "ja544:124.1": "Bahudukkhā posiya candaṁ, ",
  "ja544:124.2": "amma tuvaṁ jīyase puttaṁ; ",
  "ja544:124.3": "Vandāmi kho te pāde, ",
  "ja544:124.4": "labhataṁ tāto paralokaṁ. ",
  "ja544:125.1": "Handa ca maṁ upaguyha, ",
  "ja544:125.2": "Pāde te amma vandituṁ dehi; ",
  "ja544:125.3": "Gacchāmi dāni pavāsaṁ, ",
  "ja544:125.4": "Yaññatthāya ekarājassa. ",
  "ja544:126.1": "Handa ca maṁ upagūha, ",
  "ja544:126.2": "Pāde te amma vandituṁ dehi; ",
  "ja544:126.3": "Gacchāmi dāni pavāsaṁ, ",
  "ja544:126.4": "Mātu katvā hadayasokaṁ. ",
  "ja544:127.1": "Handa ca maṁ upagūha, ",
  "ja544:127.2": "Pāde te amma vandituṁ dehi; ",
  "ja544:127.3": "Gacchāmi dāni pavāsaṁ, ",
  "ja544:127.4": "Janassa katvā hadayasokaṁ”. ",
  "ja544:128.1": "“Handa ca padumapattānaṁ, ",
  "ja544:128.2": "Moḷiṁ bandhassu gotamiputta; ",
  "ja544:128.3": "Campakadalamissāyo, ",
  "ja544:128.4": "Esā te porāṇikā pakati. ",
  "ja544:129.1": "Handa ca vilepanaṁ te, ",
  "ja544:129.2": "Pacchimakaṁ candanaṁ vilimpassu; ",
  "ja544:129.3": "Yehi ca suvilitto, ",
  "ja544:129.4": "Sobhasi rājaparisāyaṁ. ",
  "ja544:130.1": "Handa ca mudukāni vatthāni, ",
  "ja544:130.2": "Pacchimakaṁ kāsikaṁ nivāsehi; ",
  "ja544:130.3": "Yehi ca sunivattho, ",
  "ja544:130.4": "Sobhasi rājaparisāyaṁ. ",
  "ja544:131.1": "Muttāmaṇikanakavibhūsitāni, ",
  "ja544:131.2": "Gaṇhassu hatthābharaṇāni; ",
  "ja544:131.3": "Yehi ca hatthābharaṇehi, ",
  "ja544:131.4": "Sobhasi rājaparisāyaṁ”. ",
  "ja544:132.1": "“Na hi nūnāyaṁ raṭṭhapālo, ",
  "ja544:132.2": "Bhūmipati janapadassa dāyādo; ",
  "ja544:132.3": "Lokissaro mahanto, ",
  "ja544:132.4": "Putte snehaṁ janayati”. ",
  "ja544:133.1": "“Mayhampi piyā puttā, ",
  "ja544:133.2": "Attā ca piyo tumhe ca bhariyāyo; ",
  "ja544:133.3": "Saggañca patthayāno, ",
  "ja544:133.4": "Tenāhaṁ ghātayissāmi”. ",
  "ja544:134.1": "“Maṁ paṭhamaṁ ghātehi, ",
  "ja544:134.2": "Mā me hadayaṁ dukkhaṁ phālesi; ",
  "ja544:134.3": "Alaṅkato sundarako, ",
  "ja544:134.4": "Putto deva tava sukhumālo. ",
  "ja544:135.1": "Handayya maṁ hanassu, ",
  "ja544:135.2": "Paraloke candakena hessāmi; ",
  "ja544:135.3": "Puññaṁ karassu vipulaṁ, ",
  "ja544:135.4": "Vicarāma ubhopi paraloke”. ",
  "ja544:136.1": "“Mā tvaṁ cande rucci maraṇaṁ, ",
  "ja544:136.2": "Bahukā tava devarā visālakkhi; ",
  "ja544:136.3": "Te taṁ ramayissanti, ",
  "ja544:136.4": "Yiṭṭhasmiṁ gotamiputte”. ",
  "ja544:137.1": "Evaṁ vutte, candā attānaṁ, ",
  "ja544:137.2": "hanti hatthatalakehi; ",
  "ja544:137.3": "“Alamettha jīvitena, ",
  "ja544:137.4": "pissāmi visaṁ marissāmi. ",
  "ja544:138.1": "Na hi nūnimassa rañño, ",
  "ja544:138.2": "Mittāmaccā ca vijjare suhadā; ",
  "ja544:138.3": "Ye na vadanti rājānaṁ, ",
  "ja544:138.4": "Mā ghātayi orase putte. ",
  "ja544:139.1": "Na hi nūnimassa rañño, ",
  "ja544:139.2": "Ñātī mittā ca vijjare suhadā; ",
  "ja544:139.3": "Ye na vadanti rājānaṁ, ",
  "ja544:139.4": "Mā ghātayi atraje putte. ",
  "ja544:140.1": "Ime tepi mayhaṁ puttā, ",
  "ja544:140.2": "Guṇino kāyūradhārino rāja; ",
  "ja544:140.3": "Tehipi yajassu yaññaṁ, ",
  "ja544:140.4": "Atha muñcatu gotamiputte. ",
  "ja544:141.1": "Bilasataṁ maṁ katvāna, ",
  "ja544:141.2": "Yajassu sattadhā mahārāja; ",
  "ja544:141.3": "Mā jeṭṭhaputtamavadhi, ",
  "ja544:141.4": "Adūsakaṁ sīhasaṅkāsaṁ. ",
  "ja544:142.1": "Bilasataṁ maṁ katvāna, ",
  "ja544:142.2": "Yajassu sattadhā mahārāja; ",
  "ja544:142.3": "Mā jeṭṭhaputtamavadhi, ",
  "ja544:142.4": "Apekkhitaṁ sabbalokassa”. ",
  "ja544:143.1": "“Bahukā tava dinnābharaṇā, ",
  "ja544:143.2": "Uccāvacā subhaṇitamhi; ",
  "ja544:143.3": "Muttāmaṇiveḷuriyā, ",
  "ja544:143.4": "Etaṁ te pacchimakaṁ dānaṁ”. ",
  "ja544:144.1": "“Yesaṁ pubbe khandhesu, ",
  "ja544:144.2": "Phullā mālāguṇā vivattiṁsu; ",
  "ja544:144.3": "Tesajjapi sunisito, ",
  "ja544:144.4": "Nettiṁso vivattissati khandhesu. ",
  "ja544:145.1": "Yesaṁ pubbe khandhesu, ",
  "ja544:145.2": "Cittā mālāguṇā vivattiṁsu; ",
  "ja544:145.3": "Tesajjapi sunisito, ",
  "ja544:145.4": "Nettiṁso vivattissati khandhesu. ",
  "ja544:146.1": "Aciraṁ vata nettiṁso, ",
  "ja544:146.2": "Vivattissati rājaputtānaṁ khandhesu; ",
  "ja544:146.3": "Atha mama hadayaṁ na phalati, ",
  "ja544:146.4": "Tāva daḷhabandhañca me āsi. ",
  "ja544:147.1": "Kāsikasucivatthadharā, ",
  "ja544:147.2": "Kuṇḍalino agalucandanavilittā; ",
  "ja544:147.3": "Niyyātha candasūriyā, ",
  "ja544:147.4": "Yaññatthāya ekarājassa. ",
  "ja544:148.1": "Kāsikasucivatthadharā, ",
  "ja544:148.2": "Kuṇḍalino agalucandanavilittā; ",
  "ja544:148.3": "Niyyātha candasūriyā, ",
  "ja544:148.4": "Mātu katvā hadayasokaṁ. ",
  "ja544:149.1": "Kāsikasucivatthadharā, ",
  "ja544:149.2": "Kuṇḍalino agalucandanavilittā; ",
  "ja544:149.3": "Niyyātha candasūriyā, ",
  "ja544:149.4": "Janassa katvā hadayasokaṁ. ",
  "ja544:150.1": "Maṁsarasabhojanā nhāpakasunhāpitā, ",
  "ja544:150.2": "Kuṇḍalino agalucandanavilittā; ",
  "ja544:150.3": "Niyyātha candasūriyā, ",
  "ja544:150.4": "Yaññatthāya ekarājassa. ",
  "ja544:151.1": "Maṁsarasabhojanā nhāpakasunhāpitā, ",
  "ja544:151.2": "Kuṇḍalino agalucandanavilittā; ",
  "ja544:151.3": "Niyyātha candasūriyā, ",
  "ja544:151.4": "Mātu katvā hadayasokaṁ. ",
  "ja544:152.1": "Maṁsarasabhojanā nhāpakasunhāpitā, ",
  "ja544:152.2": "Kuṇḍalino agalucandanavilittā; ",
  "ja544:152.3": "Niyyātha candasūriyā, ",
  "ja544:152.4": "Janassa katvā hadayasokaṁ”. ",
  "ja544:153.1": "Sabbasmiṁ upakkhaṭasmiṁ, ",
  "ja544:153.2": "Nisīdite candasmiṁ yaññatthāya; ",
  "ja544:153.3": "Pañcālarājadhītā pañjalikā, ",
  "ja544:153.4": "Sabbaparisāya samanupariyāyi. ",
  "ja544:154.1": "“Yena saccena khaṇḍahālo, ",
  "ja544:154.2": "Pāpakammaṁ karoti dummedho; ",
  "ja544:154.3": "Etena saccavajjena, ",
  "ja544:154.4": "Samaṅginī sāmikena homi. ",
  "ja544:155.1": "Ye idhatthi amanussā, ",
  "ja544:155.2": "Yāni ca yakkhabhūtabhabyāni; ",
  "ja544:155.3": "Karontu me veyyāvaṭikaṁ, ",
  "ja544:155.4": "Samaṅginī sāmikena homi. ",
  "ja544:156.1": "Yā devatā idhāgatā, ",
  "ja544:156.2": "Yāni ca yakkhabhūtabhabyāni; ",
  "ja544:156.3": "Saraṇesiniṁ anāthaṁ tāyatha maṁ, ",
  "ja544:156.4": "Yācāmahaṁ pati māhaṁ ajeyyaṁ”. ",
  "ja544:157.1": "Taṁ sutvā amanusso, ",
  "ja544:157.2": "Ayokūṭaṁ paribbhametvāna; ",
  "ja544:157.3": "Bhayamassa janayanto, ",
  "ja544:157.4": "Rājānaṁ idamavoca. ",
  "ja544:158.1": "“Bujjhassu kho rājakali, ",
  "ja544:158.2": "Mā tāhaṁ matthakaṁ nitāḷesiṁ; ",
  "ja544:158.3": "Mā jeṭṭhaputtamavadhi, ",
  "ja544:158.4": "Adūsakaṁ sīhasaṅkāsaṁ. ",
  "ja544:159.1": "Ko te diṭṭho rājakali, ",
  "ja544:159.2": "Puttabhariyāyo haññamānāyo; ",
  "ja544:159.3": "Seṭṭhi ca gahapatayo, ",
  "ja544:159.4": "Adūsakā saggakāmā hi”. ",
  "ja544:160.1": "Taṁ sutvā khaṇḍahālo, ",
  "ja544:160.2": "Rājā ca abbhutamidaṁ disvāna; ",
  "ja544:160.3": "Sabbesaṁ bandhanāni mocesuṁ, ",
  "ja544:160.4": "Yathā taṁ anupaghātaṁ. ",
  "ja544:161.1": "Sabbesu vippamuttesu, ",
  "ja544:161.2": "Ye tattha samāgatā tadā āsuṁ; ",
  "ja544:161.3": "Sabbe ekekaleḍḍukamadaṁsu, ",
  "ja544:161.4": "Esa vadho khaṇḍahālassa. ",
  "ja544:162.1": "Sabbe paviṭṭhā nirayaṁ, ",
  "ja544:162.2": "Yathā taṁ pāpakaṁ karitvāna; ",
  "ja544:162.3": "Na hi pāpakammaṁ katvā, ",
  "ja544:162.4": "Labbhā sugatiṁ ito gantuṁ. ",
  "ja544:163.1": "Sabbesu vippamuttesu, ",
  "ja544:163.2": "Ye tattha samāgatā tadā āsuṁ; ",
  "ja544:163.3": "Candaṁ abhisiñciṁsu, ",
  "ja544:163.4": "Samāgatā rājaparisā ca. ",
  "ja544:164.1": "Sabbesu vippamuttesu, ",
  "ja544:164.2": "Ye tattha samāgatā tadā āsuṁ; ",
  "ja544:164.3": "Candaṁ abhisiñciṁsu, ",
  "ja544:164.4": "Samāgatā rājakaññāyo ca. ",
  "ja544:165.1": "Sabbesu vippamuttesu, ",
  "ja544:165.2": "Ye tattha samāgatā tadā āsuṁ; ",
  "ja544:165.3": "Candaṁ abhisiñciṁsu, ",
  "ja544:165.4": "Samāgatā devaparisā ca. ",
  "ja544:166.1": "Sabbesu vippamuttesu, ",
  "ja544:166.2": "Ye tattha samāgatā tadā āsuṁ; ",
  "ja544:166.3": "Candaṁ abhisiñciṁsu, ",
  "ja544:166.4": "Samāgatā devakaññāyo ca. ",
  "ja544:167.1": "Sabbesu vippamuttesu, ",
  "ja544:167.2": "Ye tattha samāgatā tadā āsuṁ; ",
  "ja544:167.3": "Celukkhepamakaruṁ, ",
  "ja544:167.4": "Samāgatā rājaparisā ca. ",
  "ja544:168.1": "Sabbesu vippamuttesu, ",
  "ja544:168.2": "Ye tattha samāgatā tadā āsuṁ; ",
  "ja544:168.3": "Celukkhepamakaruṁ, ",
  "ja544:168.4": "Samāgatā rājakaññāyo ca. ",
  "ja544:169.1": "Sabbesu vippamuttesu, ",
  "ja544:169.2": "Ye tattha samāgatā tadā āsuṁ; ",
  "ja544:169.3": "Celukkhepamakaruṁ, ",
  "ja544:169.4": "Samāgatā devaparisā ca. ",
  "ja544:170.1": "Sabbesu vippamuttesu, ",
  "ja544:170.2": "Ye tattha samāgatā tadā āsuṁ; ",
  "ja544:170.3": "Celukkhepamakaruṁ, ",
  "ja544:170.4": "Samāgatā devakaññāyo ca. ",
  "ja544:171.1": "Sabbesu vippamuttesu, ",
  "ja544:171.2": "Bahū ānanditā ahuṁ; ",
  "ja544:171.3": "Nandiṁ pavesi nagaraṁ, ",
  "ja544:171.4": "Bandhanā mokkho aghositthāti. ",
  "ja544:172.1": "Candakumārajātakaṁ sattamaṁ. "
}