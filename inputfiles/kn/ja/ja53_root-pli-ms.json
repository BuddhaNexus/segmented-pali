{
"ja53:1.1": "Jātaka",
"ja53:2.1": "Ekakanipāta",
"ja53:3.1": "Āsīsavagga",
"ja53:4.1": "3. Puṇṇapātijātaka",
"ja53:5.1": "“Tatheva puṇṇā pātiyo,",
"ja53:5.2": "aññāyaṃ vattate kathā;",
"ja53:5.3": "Ākāraṇena jānāmi,",
"ja53:5.4": "na cāyaṃ bhaddikā surā”ti.",
"ja53:6.1": "Puṇṇapātijātakaṃ tatiyaṃ."
}