{
"ja301:1.1": "Jātaka",
"ja301:2.1": "Catukkanipāta",
"ja301:3.1": "Kāliṅgavagga",
"ja301:4.1": "1. Cūḷakāliṅgajātaka",
"ja301:5.1": "“Vivarathimāsaṃ dvāraṃ,",
"ja301:5.2": "Nagaraṃ pavisantu aruṇarājassa;",
"ja301:5.3": "Sīhena susiṭṭhena,",
"ja301:5.4": "Surakkhitaṃ nandisenena”.",
"ja301:6.1": "“Jayo kaliṅgānamasayhasāhinaṃ,",
"ja301:6.2": "Parājayo anayo assakānaṃ;",
"ja301:6.3": "Icceva te bhāsitaṃ brahmacāri,",
"ja301:6.4": "Na ujjubhūtā vitathaṃ bhaṇanti”.",
"ja301:7.1": "“Devā musāvādamupātivattā,",
"ja301:7.2": "Saccaṃ dhanaṃ paramaṃ tesu sakka;",
"ja301:7.3": "Taṃ te musā bhāsitaṃ devarāja,",
"ja301:7.4": "Kiṃ vā paṭicca maghavā mahinda”.",
"ja301:8.1": "“Nanu te sutaṃ brāhmaṇa bhaññamāne,",
"ja301:8.2": "Devā na issanti purisaparakkamassa;",
"ja301:8.3": "Damo samādhi manaso abhejjo,",
"ja301:8.4": "Abyaggatā nikkamanañca kāle;",
"ja301:8.5": "Daḷhañca viriyaṃ purisaparakkamo ca,",
"ja301:8.6": "Teneva āsi vijayo assakānan”ti.",
"ja301:9.1": "Cūḷakāliṅgajātakaṃ paṭhamaṃ."
}