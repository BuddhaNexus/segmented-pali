{
"ja11:1.1": "Jātaka",
"ja11:2.1": "Ekakanipāta",
"ja11:3.1": "Sīlavagga",
"ja11:4.1": "1. Lakkhaṇamigajātaka",
"ja11:5.1": "“Hoti sīlavataṃ attho,",
"ja11:5.2": "paṭisanthāravuttinaṃ;",
"ja11:5.3": "Lakkhaṇaṃ passa āyantaṃ,",
"ja11:5.4": "ñātisaṅghapurakkhataṃ;",
"ja11:5.5": "Atha passasimaṃ kāḷaṃ,",
"ja11:5.6": "suvihīnaṃva ñātibhī”ti.",
"ja11:6.1": "Lakkhaṇamigajātakaṃ paṭhamaṃ."
}