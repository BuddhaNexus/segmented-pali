{
"ja73:1.1": "Jātaka",
"ja73:2.1": "Ekakanipāta",
"ja73:3.1": "Varuṇavagga",
"ja73:4.1": "3. Saccaṃkirajātaka",
"ja73:5.1": "“Saccaṃ kirevamāhaṃsu,",
"ja73:5.2": "narā ekacciyā idha;",
"ja73:5.3": "Kaṭṭhaṃ niplavitaṃ seyyo,",
"ja73:5.4": "na tvevekacciyo naro”ti.",
"ja73:6.1": "Saccaṃkirajātakaṃ tatiyaṃ."
}