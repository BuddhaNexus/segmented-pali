{
"ja436:1.1": "Jātaka",
"ja436:2.1": "Navakanipāta",
"ja436:3.1": "Gijjhavagga",
"ja436:4.1": "10. Samuggajātaka",
"ja436:5.1": "“Kuto nu āgacchatha bho tayo janā,",
"ja436:5.2": "Svāgatā etha nisīdathāsane;",
"ja436:5.3": "Kaccittha bhonto kusalaṃ anāmayaṃ,",
"ja436:5.4": "Cirassamabbhāgamanaṃ hi vo idha”.",
"ja436:6.1": "“Ahameva eko idha majja patto,",
"ja436:6.2": "Na cāpi me dutiyo koci vijjati;",
"ja436:6.3": "Kimeva sandhāya te bhāsitaṃ ise,",
"ja436:6.4": "‘Kuto nu āgacchatha bho tayo janā’”.",
"ja436:7.1": "“Tuvañca eko bhariyā ca te piyā,",
"ja436:7.2": "Samuggapakkhittanikiṇṇamantare;",
"ja436:7.3": "Sā rakkhitā kucchigatāva te sadā,",
"ja436:7.4": "Vāyussa puttena sahā tahiṃ ratā”.",
"ja436:8.1": "“Saṃviggarūpo isinā viyākato,",
"ja436:8.2": "So dānavo tattha samuggamuggili;",
"ja436:8.3": "Addakkhi bhariyaṃ suci māladhāriniṃ,",
"ja436:8.4": "Vāyussa puttena sahā tahiṃ rataṃ”.",
"ja436:9.1": "“Sudiṭṭharūpamuggatapānuvattinā,",
"ja436:9.2": "Hīnā narā ye pamadāvasaṃ gatā;",
"ja436:9.3": "Yathā have pāṇarivettha rakkhitā,",
"ja436:9.4": "Duṭṭhā mayī aññamabhippamodayi.",
"ja436:10.1": "Divā ca ratto ca mayā upaṭṭhitā,",
"ja436:10.2": "Tapassinā jotirivā vane vasaṃ;",
"ja436:10.3": "Sā dhammamukkamma adhammamācari,",
"ja436:10.4": "Akiriyarūpo pamadāhi santhavo.",
"ja436:11.1": "Sarīramajjhamhi ṭhitātimaññahaṃ,",
"ja436:11.2": "Mayhaṃ ayanti asatiṃ asaññataṃ;",
"ja436:11.3": "Sā dhammamukkamma adhammamācari,",
"ja436:11.4": "Akiriyarūpo pamadāhi santhavo.",
"ja436:12.1": "Surakkhitaṃ meti kathaṃ nu vissase,",
"ja436:12.2": "Anekacittāsu na hatthi rakkhaṇā;",
"ja436:12.3": "Etā hi pātālapapātasannibhā,",
"ja436:12.4": "Etthappamatto byasanaṃ nigaccha”ti.",
"ja436:13.1": "“Tasmā hi te sukhino vītasokā,",
"ja436:13.2": "Ye mātugāmehi caranti nissaṭā;",
"ja436:13.3": "Etaṃ sivaṃ uttamamābhipatthayaṃ,",
"ja436:13.4": "Na mātugāmehi kareyya santhavanti”.",
"ja436:14.1": "Samuggajātakaṃ dasamaṃ."
}