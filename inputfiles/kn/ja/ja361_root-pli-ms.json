{
"ja361:1.1": "Jātaka",
"ja361:2.1": "Pañcakanipāta",
"ja361:3.1": "Vaṇṇārohavagga",
"ja361:4.1": "1. Vaṇṇārohajātaka",
"ja361:5.1": "“Vaṇṇārohena jātiyā,",
"ja361:5.2": "balanikkamanena ca;",
"ja361:5.3": "Subāhu na mayā seyyo,",
"ja361:5.4": "sudāṭha iti bhāsasi”.",
"ja361:6.1": "“Vaṇṇārohena jātiyā,",
"ja361:6.2": "balanikkamanena ca;",
"ja361:6.3": "Sudāṭho na mayā seyyo,",
"ja361:6.4": "subāhu iti bhāsasi.",
"ja361:7.1": "Evañce maṃ viharantaṃ,",
"ja361:7.2": "subāhu samma dubbhasi;",
"ja361:7.3": "Na dānāhaṃ tayā saddhiṃ,",
"ja361:7.4": "saṃvāsamabhirocaye.",
"ja361:8.1": "Yo paresaṃ vacanāni,",
"ja361:8.2": "saddaheyya yathātathaṃ;",
"ja361:8.3": "Khippaṃ bhijjetha mittasmiṃ,",
"ja361:8.4": "verañca pasave bahuṃ.",
"ja361:9.1": "Na so mitto yo sadā appamatto,",
"ja361:9.2": "Bhedāsaṅkī randhamevānupassī;",
"ja361:9.3": "Yasmiñca setī urasīva putto,",
"ja361:9.4": "Sa ve mitto yo abhejjo parehī”ti.",
"ja361:10.1": "Vaṇṇārohajātakaṃ paṭhamaṃ."
}