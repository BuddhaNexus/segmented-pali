{
"ja117:1.1": "Jātaka",
"ja117:2.1": "Ekakanipāta",
"ja117:3.1": "Haṃcivagga",
"ja117:4.1": "7. Tittirajātaka",
"ja117:5.1": "“Accuggatātilapatā,",
"ja117:5.2": "ativelaṃ pabhāsitā;",
"ja117:5.3": "Vācā hanati dummedhaṃ,",
"ja117:5.4": "tittiraṃvātivassitan”ti.",
"ja117:6.1": "Tittirajātakaṃ sattamaṃ."
}