{
"ja91:1.1": "Jātaka",
"ja91:2.1": "Ekakanipāta",
"ja91:3.1": "Littavagga",
"ja91:4.1": "1. Littajātaka",
"ja91:5.1": "“Littaṃ paramena tejasā,",
"ja91:5.2": "Gilamakkhaṃ puriso na bujjhati;",
"ja91:5.3": "Gila re gila pāpadhuttaka,",
"ja91:5.4": "Pacchā te kaṭukaṃ bhavissatī”ti.",
"ja91:6.1": "Littajātakaṃ paṭhamaṃ."
}