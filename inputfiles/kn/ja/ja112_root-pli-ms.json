{
"ja112:1.1": "Jātaka",
"ja112:2.1": "Ekakanipāta",
"ja112:3.1": "Haṃcivagga",
"ja112:4.1": "2. Amarādevīpañhajātaka",
"ja112:5.1": "“Yena sattubilaṅgā ca,",
"ja112:5.2": "Diguṇapalāso ca pupphito;",
"ja112:5.3": "Yena dadāmi tena vadāmi,",
"ja112:5.4": "Yena na dadāmi na tena vadāmi;",
"ja112:5.5": "Esa maggo yavamajjhakassa,",
"ja112:5.6": "Etaṃ channapathaṃ vijānāhī”ti.",
"ja112:6.1": "Amarādevīpañhajātakaṃ dutiyaṃ."
}