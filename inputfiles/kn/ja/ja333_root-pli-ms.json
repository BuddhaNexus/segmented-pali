{
"ja333:1.1": "Jātaka",
"ja333:2.1": "Catukkanipāta",
"ja333:3.1": "Kokilavagga",
"ja333:4.1": "3. Pakkagodhajātaka",
"ja333:5.1": "“Tadeva me tvaṃ vidito,",
"ja333:5.2": "vanamajjhe rathesabha;",
"ja333:5.3": "Yassa te khaggabaddhassa,",
"ja333:5.4": "sannaddhassa tirīṭino;",
"ja333:5.5": "Assatthadumasākhāya,",
"ja333:5.6": "pakkā godhā palāyatha”.",
"ja333:6.1": "“Name namantassa bhaje bhajantaṃ,",
"ja333:6.2": "Kiccānukubbassa kareyya kiccaṃ;",
"ja333:6.3": "Nānatthakāmassa kareyya atthaṃ,",
"ja333:6.4": "Asambhajantampi na sambhajeyya.",
"ja333:7.1": "Caje cajantaṃ vanathaṃ na kayirā,",
"ja333:7.2": "Apetacittena na sambhajeyya;",
"ja333:7.3": "Dijo dumaṃ khīṇaphalanti ñatvā,",
"ja333:7.4": "Aññaṃ samekkheyya mahā hi loko”.",
"ja333:8.1": "“So te karissāmi yathānubhāvaṃ,",
"ja333:8.2": "Kataññutaṃ khattiye pekkhamāno;",
"ja333:8.3": "Sabbañca te issariyaṃ dadāmi,",
"ja333:8.4": "Yassicchasī tassa tuvaṃ dadāmī”ti.",
"ja333:9.1": "Pakkagodhajātakaṃ tatiyaṃ."
}