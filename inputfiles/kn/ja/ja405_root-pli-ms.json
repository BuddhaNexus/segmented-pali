{
"ja405:1.1": "Jātaka",
"ja405:2.1": "Sattakanipāta",
"ja405:3.1": "Kukkuvagga",
"ja405:4.1": "10. Bakajātaka",
"ja405:5.1": "“Dvāsattati gotama puññakammā,",
"ja405:5.2": "Vasavattino jātijaraṃ atītā;",
"ja405:5.3": "Ayamantimā vedagū brahmapatti,",
"ja405:5.4": "Asmābhijappanti janā anekā”.",
"ja405:6.1": "“Appañhi etaṃ na hi dīghamāyu,",
"ja405:6.2": "Yaṃ tvaṃ baka maññasi dīghamāyuṃ;",
"ja405:6.3": "Sataṃ sahassāni nirabbudānaṃ,",
"ja405:6.4": "Āyuṃ pajānāmi tavāha brahme”.",
"ja405:7.1": "“Anantadassī bhagavāhamasmi,",
"ja405:7.2": "Jātijjaraṃ sokamupātivatto;",
"ja405:7.3": "Kiṃ me purāṇaṃ vatasīlavattaṃ,",
"ja405:7.4": "Ācikkha me taṃ yamahaṃ vijaññaṃ”.",
"ja405:8.1": "“Yaṃ tvaṃ apāyesi bahū manusse,",
"ja405:8.2": "Pipāsite ghammani samparete;",
"ja405:8.3": "Taṃ te purāṇaṃ vatasīlavattaṃ,",
"ja405:8.4": "Suttappabuddhova anussarāmi.",
"ja405:9.1": "Yaṃ eṇikūlasmi janaṃ gahītaṃ,",
"ja405:9.2": "Amocayī gayhaka niyyamānaṃ;",
"ja405:9.3": "Taṃ te purāṇaṃ vatasīlavattaṃ,",
"ja405:9.4": "Suttappabuddhova anussarāmi.",
"ja405:10.1": "Gaṅgāya sotasmiṃ gahītanāvaṃ,",
"ja405:10.2": "Luddena nāgena manussakappā;",
"ja405:10.3": "Amocayi tvaṃ balasā pasayha,",
"ja405:10.4": "Taṃ te purāṇaṃ vatasīlavattaṃ;",
"ja405:10.5": "Suttappabuddhova anussarāmi.",
"ja405:11.1": "Kappo ca te baddhacaro ahosiṃ,",
"ja405:11.2": "Sambuddhimantaṃ vatinaṃ amaññaṃ;",
"ja405:11.3": "Taṃ te purāṇaṃ vatasīlavattaṃ,",
"ja405:11.4": "Suttappabuddhova anussarāmi”.",
"ja405:12.1": "“Addhā pajānāsi mametamāyuṃ,",
"ja405:12.2": "Aññampi jānāsi tathā hi buddho;",
"ja405:12.3": "Tathā hi tāyaṃ jalitānubhāvo,",
"ja405:12.4": "Obhāsayaṃ tiṭṭhati brahmalokan”ti.",
"ja405:13.1": "Bakajātakaṃ dasamaṃ.",
"ja405:13.2": "Kukkuvaggo paṭhamo.",
"ja405:14.1": "Tassuddānaṃ",
"ja405:15.1": "Varakaṇṇika cāpavaro sutano,",
"ja405:15.2": "Atha gijjha sarohitamacchavaro;",
"ja405:15.3": "Puna paṇṇaka senaka yācanako,",
"ja405:15.4": "Atha veri sabrahmabakena dasāti."
}