{
  "ja388:0.1": "Jātaka ",
  "ja388:0.2": "Chakkanipāta ",
  "ja388:0.3": "Kharaputtavagga ",
  "ja388:0.4": "3. Tuṇḍilajātaka ",
  "ja388:1.1": "“Navachannake dāni diyyati, ",
  "ja388:1.2": "Puṇṇāyaṁ doṇi suvāminī ṭhitā; ",
  "ja388:1.3": "Bahuke jane pāsapāṇike, ",
  "ja388:1.4": "No ca kho me paṭibhāti bhuñjituṁ”. ",
  "ja388:2.1": "“Tasasi bhamasi leṇamicchasi, ",
  "ja388:2.2": "Attāṇosi kuhiṁ gamissasi; ",
  "ja388:2.3": "Appossukko bhuñja tuṇḍila, ",
  "ja388:2.4": "Maṁsatthāya hi positāmhase. ",
  "ja388:3.1": "Ogaha rahadaṁ akaddamaṁ, ",
  "ja388:3.2": "Sabbaṁ sedamalaṁ pavāhaya; ",
  "ja388:3.3": "Gaṇhāhi navaṁ vilepanaṁ, ",
  "ja388:3.4": "Yassa gandho na kadāci chijjati”. ",
  "ja388:4.1": "“Katamo rahado akaddamo, ",
  "ja388:4.2": "Kiṁsu sedamalanti vuccati; ",
  "ja388:4.3": "Katamañca navaṁ vilepanaṁ, ",
  "ja388:4.4": "Yassa gandho na kadāci chijjati”. ",
  "ja388:5.1": "“Dhammo rahado akaddamo, ",
  "ja388:5.2": "Pāpaṁ sedamalanti vuccati; ",
  "ja388:5.3": "Sīlañca navaṁ vilepanaṁ, ",
  "ja388:5.4": "Tassa gandho na kadāci chijjati. ",
  "ja388:6.1": "Nandanti sarīraghātino, ",
  "ja388:6.2": "Na ca nandanti sarīradhārino; ",
  "ja388:6.3": "Puṇṇāya ca puṇṇamāsiyā, ",
  "ja388:6.4": "Ramamānāva jahanti jīvitan”ti. ",
  "ja388:7.1": "Tuṇḍilajātakaṁ tatiyaṁ. "
}