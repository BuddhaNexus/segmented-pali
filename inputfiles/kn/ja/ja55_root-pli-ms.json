{
  "ja55:0.1": "Jātaka ",
  "ja55:0.2": "Ekakanipāta ",
  "ja55:0.3": "Āsīsavagga ",
  "ja55:0.4": "5. Pañcāvudhajātaka ",
  "ja55:1.1": "“Yo alīnena cittena, ",
  "ja55:1.2": "alīnamanaso naro; ",
  "ja55:1.3": "Bhāveti kusalaṁ dhammaṁ, ",
  "ja55:1.4": "yogakkhemassa pattiyā; ",
  "ja55:1.5": "Pāpuṇe anupubbena, ",
  "ja55:1.6": "sabbasaṁyojanakkhayan”ti. ",
  "ja55:2.1": "Pañcāvudhajātakaṁ pañcamaṁ. "
}