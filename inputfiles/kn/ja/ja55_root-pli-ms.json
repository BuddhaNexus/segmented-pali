{
"ja55:1.1": "Jātaka",
"ja55:2.1": "Ekakanipāta",
"ja55:3.1": "Āsīsavagga",
"ja55:4.1": "5. Pañcāvudhajātaka",
"ja55:5.1": "“Yo alīnena cittena,",
"ja55:5.2": "alīnamanaso naro;",
"ja55:5.3": "Bhāveti kusalaṃ dhammaṃ,",
"ja55:5.4": "yogakkhemassa pattiyā;",
"ja55:5.5": "Pāpuṇe anupubbena,",
"ja55:5.6": "sabbasaṃyojanakkhayan”ti.",
"ja55:6.1": "Pañcāvudhajātakaṃ pañcamaṃ."
}