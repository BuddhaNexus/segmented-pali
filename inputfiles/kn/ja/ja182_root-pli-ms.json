{
"ja182:1.1": "Jātaka",
"ja182:2.1": "Dukanipāta",
"ja182:3.1": "Asadisavagga",
"ja182:4.1": "2. Saṅgāmāvacarajātaka",
"ja182:5.1": "“Saṅgāmāvacaro sūro,",
"ja182:5.2": "balavā iti vissuto;",
"ja182:5.3": "Kiṃ nu toraṇamāsajja,",
"ja182:5.4": "paṭikkamasi kuñjara.",
"ja182:6.1": "Omadda khippaṃ palighaṃ,",
"ja182:6.2": "esikāni ca abbaha;",
"ja182:6.3": "Toraṇāni ca madditvā,",
"ja182:6.4": "khippaṃ pavisa kuñjarā”ti.",
"ja182:7.1": "Saṅgāmāvacarajātakaṃ dutiyaṃ."
}