{
  "ja140:0.1": "Jātaka ",
  "ja140:0.2": "Ekakanipāta ",
  "ja140:0.3": "Asampadānavagga ",
  "ja140:0.4": "10. Kākajātaka ",
  "ja140:1.1": "“Niccaṁ ubbiggahadayā, ",
  "ja140:1.2": "sabbalokavihesakā; ",
  "ja140:1.3": "Tasmā nesaṁ vasā natthi, ",
  "ja140:1.4": "kākānamhāka ñātinan”ti. ",
  "ja140:2.1": "Kākajātakaṁ dasamaṁ. ",
  "ja140:2.2": "Asampadānavaggo cuddasamo. ",
  "ja140:3.1": "Tassuddānaṁ ",
  "ja140:4.1": "Itarītara rakkhasi khemiyo ca, ",
  "ja140:4.2": "Parosatapañhena ābhassaro puna; ",
  "ja140:4.3": "Atha haṁsavaruttamababbujaṭaṁ, ",
  "ja140:4.4": "Paṭanaṭṭhaka kākavarena dasāti. "
}