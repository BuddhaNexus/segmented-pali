{
"ja140:1.1": "Jātaka",
"ja140:2.1": "Ekakanipāta",
"ja140:3.1": "Asampadānavagga",
"ja140:4.1": "10. Kākajātaka",
"ja140:5.1": "“Niccaṃ ubbiggahadayā,",
"ja140:5.2": "sabbalokavihesakā;",
"ja140:5.3": "Tasmā nesaṃ vasā natthi,",
"ja140:5.4": "kākānamhāka ñātinan”ti.",
"ja140:6.1": "Kākajātakaṃ dasamaṃ.",
"ja140:6.2": "Asampadānavaggo cuddasamo.",
"ja140:7.1": "Tassuddānaṃ",
"ja140:8.1": "Itarītara rakkhasi khemiyo ca,",
"ja140:8.2": "Parosatapañhena ābhassaro puna;",
"ja140:8.3": "Atha haṃsavaruttamababbujaṭaṃ,",
"ja140:8.4": "Paṭanaṭṭhaka kākavarena dasāti."
}