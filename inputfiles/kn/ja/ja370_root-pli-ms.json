{
  "ja370:0.1": "Jātaka ",
  "ja370:0.2": "Pañcakanipāta ",
  "ja370:0.3": "Vaṇṇārohavagga ",
  "ja370:0.4": "10. Palāsajātaka ",
  "ja370:1.1": "“Haṁso palāsamavaca, ",
  "ja370:1.2": "nigrodho samma jāyati; ",
  "ja370:1.3": "Aṅkasmiṁ te nisinnova, ",
  "ja370:1.4": "so te mammāni checchati”. ",
  "ja370:2.1": "“Vaḍḍhatāmeva nigrodho, ",
  "ja370:2.2": "patiṭṭhassa bhavāmahaṁ; ",
  "ja370:2.3": "Yathā pitā ca mātā ca, ",
  "ja370:2.4": "evaṁ me so bhavissati”. ",
  "ja370:3.1": "“Yaṁ tvaṁ aṅkasmiṁ vaḍḍhesi, ",
  "ja370:3.2": "khīrarukkhaṁ bhayānakaṁ; ",
  "ja370:3.3": "Āmanta kho taṁ gacchāma, ",
  "ja370:3.4": "vuḍḍhi massa na ruccati”. ",
  "ja370:4.1": "“Idāni kho maṁ bhāyeti, ",
  "ja370:4.2": "mahānerunidassanaṁ; ",
  "ja370:4.3": "Haṁsassa anabhiññāya, ",
  "ja370:4.4": "mahā me bhayamāgataṁ”. ",
  "ja370:5.1": "“Na tassa vuḍḍhi kusalappasatthā, ",
  "ja370:5.2": "Yo vaḍḍhamāno ghasate patiṭṭhaṁ; ",
  "ja370:5.3": "Tassūparodhaṁ parisaṅkamāno, ",
  "ja370:5.4": "Patārayī mūlavadhāya dhīro”ti. ",
  "ja370:6.1": "Palāsajātakaṁ dasamaṁ. ",
  "ja370:6.2": "Vaṇṇārohavaggo dutiyo. "
}