{
"ja285:1.1": "Jātaka",
"ja285:2.1": "Tikanipāta",
"ja285:3.1": "Abbhantaravagga",
"ja285:4.1": "5. Maṇisūkarajātaka",
"ja285:5.1": "“Dariyā satta vassāni,",
"ja285:5.2": "tiṃsamattā vasāmase;",
"ja285:5.3": "Haññāma maṇino ābhaṃ,",
"ja285:5.4": "iti no mantanaṃ ahu.",
"ja285:6.1": "Yāvatā maṇiṃ ghaṃsāma,",
"ja285:6.2": "bhiyyo vodāyate maṇi;",
"ja285:6.3": "Idañca dāni pucchāma,",
"ja285:6.4": "kiṃ kiccaṃ idha maññasi”.",
"ja285:7.1": "“Ayaṃ maṇi veḷūriyo,",
"ja285:7.2": "akāco vimalo subho;",
"ja285:7.3": "Nāssa sakkā siriṃ hantuṃ,",
"ja285:7.4": "apakkamatha sūkarā”ti.",
"ja285:8.1": "Maṇisūkarajātakaṃ pañcamaṃ."
}