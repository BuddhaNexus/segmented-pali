{
  "ja380:0.1": "Jātaka ",
  "ja380:0.2": "Chakkanipāta ",
  "ja380:0.3": "Avāriyavagga ",
  "ja380:0.4": "5. Āsaṅkajātaka ",
  "ja380:1.1": "“Āsāvatī nāma latā, ",
  "ja380:1.2": "jātā cittalatāvane; ",
  "ja380:1.3": "Tassā vassasahassena, ",
  "ja380:1.4": "ekaṁ nibbattate phalaṁ. ",
  "ja380:2.1": "Taṁ devā payirupāsanti, ",
  "ja380:2.2": "tāva dūraphalaṁ satiṁ; ",
  "ja380:2.3": "Āsīseva tuvaṁ rāja, ",
  "ja380:2.4": "āsā phalavatī sukhā. ",
  "ja380:3.1": "Āsīsateva so pakkhī, ",
  "ja380:3.2": "āsīsateva so dijo; ",
  "ja380:3.3": "Tassa cāsā samijjhati, ",
  "ja380:3.4": "tāva dūragatā satī; ",
  "ja380:3.5": "Āsīseva tuvaṁ rāja, ",
  "ja380:3.6": "āsā phalavatī sukhā”. ",
  "ja380:4.1": "“Sampesi kho maṁ vācāya, ",
  "ja380:4.2": "na ca sampesi kammunā; ",
  "ja380:4.3": "Mālā sereyyakasseva, ",
  "ja380:4.4": "vaṇṇavantā agandhikā. ",
  "ja380:5.1": "Aphalaṁ madhuraṁ vācaṁ, ",
  "ja380:5.2": "yo mittesu pakubbati; ",
  "ja380:5.3": "Adadaṁ avissajaṁ bhogaṁ, ",
  "ja380:5.4": "sandhi tenassa jīrati. ",
  "ja380:6.1": "Yañhi kayirā tañhi vade, ",
  "ja380:6.2": "yaṁ na kayirā na taṁ vade; ",
  "ja380:6.3": "Akarontaṁ bhāsamānaṁ, ",
  "ja380:6.4": "parijānanti paṇḍitā. ",
  "ja380:7.1": "Balañca vata me khīṇaṁ, ",
  "ja380:7.2": "pātheyyañca na vijjati; ",
  "ja380:7.3": "Saṅke pāṇūparodhāya, ",
  "ja380:7.4": "handa dāni vajāmahaṁ”. ",
  "ja380:8.1": "“Etadeva hi me nāmaṁ, ",
  "ja380:8.2": "Yaṁ nāmasmi rathesabha; ",
  "ja380:8.3": "Āgamehi mahārāja, ",
  "ja380:8.4": "Pitaraṁ āmantayāmahan”ti. ",
  "ja380:9.1": "Āsaṅkajātakaṁ pañcamaṁ. "
}