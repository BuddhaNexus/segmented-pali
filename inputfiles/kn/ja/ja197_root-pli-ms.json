{
"ja197:1.1": "Jātaka",
"ja197:2.1": "Dukanipāta",
"ja197:3.1": "Ruhakavagga",
"ja197:4.1": "7. Mittāmittajātaka",
"ja197:5.1": "“Na naṃ umhayate disvā,",
"ja197:5.2": "na ca naṃ paṭinandati;",
"ja197:5.3": "Cakkhūni cassa na dadāti,",
"ja197:5.4": "paṭilomañca vattati.",
"ja197:6.1": "Ete bhavanti ākārā,",
"ja197:6.2": "amittasmiṃ patiṭṭhitā;",
"ja197:6.3": "Yehi amittaṃ jāneyya,",
"ja197:6.4": "disvā sutvā ca paṇḍito”ti.",
"ja197:7.1": "Mittāmittajātakaṃ sattamaṃ."
}