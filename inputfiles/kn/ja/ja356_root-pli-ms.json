{
"ja356:1.1": "Jātaka",
"ja356:2.1": "Pañcakanipāta",
"ja356:3.1": "Maṇikuṇḍalavagga",
"ja356:4.1": "6. Koraṇḍiyajātaka",
"ja356:5.1": "“Eko araññe girikandarāyaṃ,",
"ja356:5.2": "Paggayha paggayha silaṃ pavecchasi;",
"ja356:5.3": "Punappunaṃ santaramānarūpo,",
"ja356:5.4": "Koraṇḍiya ko nu tava yidhattho”.",
"ja356:6.1": "“Ahañhimaṃ sāgara sevitantaṃ,",
"ja356:6.2": "Samaṃ karissāmi yathāpi pāṇi;",
"ja356:6.3": "Vikiriya sānūni ca pabbatāni ca,",
"ja356:6.4": "Tasmā silaṃ dariyā pakkhipāmi”.",
"ja356:7.1": "“Nayimaṃ mahiṃ arahati pāṇikappaṃ,",
"ja356:7.2": "Samaṃ manusso karaṇāya meko;",
"ja356:7.3": "Maññāmimaññeva dariṃ jigīsaṃ,",
"ja356:7.4": "Koraṇḍiya hāhasi jīvalokaṃ”.",
"ja356:8.1": "“Sace ahaṃ bhūtadharaṃ na sakkā,",
"ja356:8.2": "Samaṃ manusso karaṇāya meko;",
"ja356:8.3": "Evameva tvaṃ brahme ime manusse,",
"ja356:8.4": "Nānādiṭṭhike nānayissasi te”.",
"ja356:9.1": "“Saṃkhittarūpena bhavaṃ mamatthaṃ,",
"ja356:9.2": "Akkhāsi koraṇḍiya evametaṃ;",
"ja356:9.3": "Yathā na sakkā pathavī samāyaṃ,",
"ja356:9.4": "Kattuṃ manussena tathā manussā”ti.",
"ja356:10.1": "Koraṇḍiyajātakaṃ chaṭṭhaṃ."
}