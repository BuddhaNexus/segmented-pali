{
  "ja356:0.1": "Jātaka ",
  "ja356:0.2": "Pañcakanipāta ",
  "ja356:0.3": "Maṇikuṇḍalavagga ",
  "ja356:0.4": "6. Koraṇḍiyajātaka ",
  "ja356:1.1": "“Eko araññe girikandarāyaṁ, ",
  "ja356:1.2": "Paggayha paggayha silaṁ pavecchasi; ",
  "ja356:1.3": "Punappunaṁ santaramānarūpo, ",
  "ja356:1.4": "Koraṇḍiya ko nu tava yidhattho”. ",
  "ja356:2.1": "“Ahañhimaṁ sāgara sevitantaṁ, ",
  "ja356:2.2": "Samaṁ karissāmi yathāpi pāṇi; ",
  "ja356:2.3": "Vikiriya sānūni ca pabbatāni ca, ",
  "ja356:2.4": "Tasmā silaṁ dariyā pakkhipāmi”. ",
  "ja356:3.1": "“Nayimaṁ mahiṁ arahati pāṇikappaṁ, ",
  "ja356:3.2": "Samaṁ manusso karaṇāya meko; ",
  "ja356:3.3": "Maññāmimaññeva dariṁ jigīsaṁ, ",
  "ja356:3.4": "Koraṇḍiya hāhasi jīvalokaṁ”. ",
  "ja356:4.1": "“Sace ahaṁ bhūtadharaṁ na sakkā, ",
  "ja356:4.2": "Samaṁ manusso karaṇāya meko; ",
  "ja356:4.3": "Evameva tvaṁ brahme ime manusse, ",
  "ja356:4.4": "Nānādiṭṭhike nānayissasi te”. ",
  "ja356:5.1": "“Saṅkhittarūpena bhavaṁ mamatthaṁ, ",
  "ja356:5.2": "Akkhāsi koraṇḍiya evametaṁ; ",
  "ja356:5.3": "Yathā na sakkā pathavī samāyaṁ, ",
  "ja356:5.4": "Kattuṁ manussena tathā manussā”ti. ",
  "ja356:6.1": "Koraṇḍiyajātakaṁ chaṭṭhaṁ. "
}