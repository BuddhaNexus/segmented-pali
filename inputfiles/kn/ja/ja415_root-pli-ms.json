{
  "ja415:0.1": "Jātaka ",
  "ja415:0.2": "Sattakanipāta ",
  "ja415:0.3": "Gandhāravagga ",
  "ja415:0.4": "10. Kummāsapiṇḍijātaka ",
  "ja415:1.1": "“Na kiratthi anomadassisu, ",
  "ja415:1.2": "Pāricariyā buddhesu appikā; ",
  "ja415:1.3": "Sukkhāya aloṇikāya ca, ",
  "ja415:1.4": "Passaphalaṁ kummāsapiṇḍiyā. ",
  "ja415:2.1": "Hatthigavassā cime bahū, ",
  "ja415:2.2": "Dhanadhaññaṁ pathavī ca kevalā; ",
  "ja415:2.3": "Nāriyo cimā accharūpamā, ",
  "ja415:2.4": "Passa phalaṁ kummāsapiṇḍiyā”. ",
  "ja415:3.1": "“Abhikkhaṇaṁ rājakuñjara, ",
  "ja415:3.2": "Gāthā bhāsasi kosalādhipa; ",
  "ja415:3.3": "Pucchāmi taṁ raṭṭhavaḍḍhana, ",
  "ja415:3.4": "Bāḷhaṁ pītimano pabhāsasi”. ",
  "ja415:4.1": "“Imasmiññeva nagare, ",
  "ja415:4.2": "kule aññatare ahuṁ; ",
  "ja415:4.3": "Parakammakaro āsiṁ, ",
  "ja415:4.4": "bhatako sīlasaṁvuto. ",
  "ja415:5.1": "Kammāya nikkhamantohaṁ, ",
  "ja415:5.2": "caturo samaṇeddasaṁ; ",
  "ja415:5.3": "Ācārasīlasampanne, ",
  "ja415:5.4": "sītibhūte anāsave. ",
  "ja415:6.1": "Tesu cittaṁ pasādetvā, ",
  "ja415:6.2": "nisīdetvā paṇṇasanthate; ",
  "ja415:6.3": "Adaṁ buddhāna kummāsaṁ, ",
  "ja415:6.4": "pasanno sehi pāṇibhi. ",
  "ja415:7.1": "Tassa kammassa kusalassa, ",
  "ja415:7.2": "idaṁ me edisaṁ phalaṁ; ",
  "ja415:7.3": "Anubhomi idaṁ rajjaṁ, ",
  "ja415:7.4": "phītaṁ dharaṇimuttamaṁ”. ",
  "ja415:8.1": "“Dadaṁ bhuñja mā ca pamādo, ",
  "ja415:8.2": "Cakkaṁ vattaya kosalādhipa; ",
  "ja415:8.3": "Mā rāja adhammiko ahu, ",
  "ja415:8.4": "Dhammaṁ pālaya kosalādhipa”. ",
  "ja415:9.1": "“Sohaṁ tadeva punappunaṁ, ",
  "ja415:9.2": "Vaṭumaṁ ācarissāmi sobhane; ",
  "ja415:9.3": "Ariyācaritaṁ sukosale, ",
  "ja415:9.4": "Arahanto me manāpāva passituṁ. ",
  "ja415:10.1": "Devī viya accharūpamā, ",
  "ja415:10.2": "Majjhe nārigaṇassa sobhasi; ",
  "ja415:10.3": "Kiṁ kammamakāsi bhaddakaṁ, ",
  "ja415:10.4": "Kenāsi vaṇṇavatī sukosale”. ",
  "ja415:11.1": "“Ambaṭṭhakulassa khattiya, ",
  "ja415:11.2": "Dāsyāhaṁ parapesiyā ahuṁ; ",
  "ja415:11.3": "Saññatā ca dhammajīvinī, ",
  "ja415:11.4": "Sīlavatī ca apāpadassanā. ",
  "ja415:12.1": "Uddhaṭabhattaṁ ahaṁ tadā, ",
  "ja415:12.2": "Caramānassa adāsi bhikkhuno; ",
  "ja415:12.3": "Vittā sumanā sayaṁ ahaṁ, ",
  "ja415:12.4": "Tassa kammassa phalaṁ mamedisan”ti. ",
  "ja415:13.1": "Kummāsapiṇḍijātakaṁ dasamaṁ. "
}