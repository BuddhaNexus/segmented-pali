{
"ja419:1.1": "Jātaka",
"ja419:2.1": "Aṭṭhakanipāta",
"ja419:3.1": "Kaccānivagga",
"ja419:4.1": "3. Sulasājātaka",
"ja419:5.1": "“Idaṃ suvaṇṇakāyūraṃ,",
"ja419:5.2": "muttā veḷuriyā bahū;",
"ja419:5.3": "Sabbaṃ harassu bhaddante,",
"ja419:5.4": "mañca dāsīti sāvaya”.",
"ja419:6.1": "“Oropayassu kalyāṇi,",
"ja419:6.2": "mā bāḷhaṃ paridevasi;",
"ja419:6.3": "Na cāhaṃ abhijānāmi,",
"ja419:6.4": "ahantvā dhanamābhataṃ”.",
"ja419:7.1": "“Yato sarāmi attānaṃ,",
"ja419:7.2": "yato pattāsmi viññutaṃ;",
"ja419:7.3": "Na cāhaṃ abhijānāmi,",
"ja419:7.4": "aññaṃ piyataraṃ tayā.",
"ja419:8.1": "Ehi taṃ upagūhissaṃ,",
"ja419:8.2": "karissañca padakkhiṇaṃ;",
"ja419:8.3": "Na hi dāni puna atthi,",
"ja419:8.4": "mama tuyhañca saṅgamo”.",
"ja419:9.1": "“Na hi sabbesu ṭhānesu,",
"ja419:9.2": "puriso hoti paṇḍito;",
"ja419:9.3": "Itthīpi paṇḍitā hoti,",
"ja419:9.4": "tattha tattha vicakkhaṇā.",
"ja419:10.1": "Na hi sabbesu ṭhānesu,",
"ja419:10.2": "puriso hoti paṇḍito;",
"ja419:10.3": "Itthīpi paṇḍitā hoti,",
"ja419:10.4": "lahuṃ atthaṃ vicintikā.",
"ja419:11.1": "Lahuñca vata khippañca,",
"ja419:11.2": "nikaṭṭhe samacetayi;",
"ja419:11.3": "Migaṃ puṇṇāyateneva,",
"ja419:11.4": "sulasā sattukaṃ vadhi.",
"ja419:12.1": "Yodha uppatitaṃ atthaṃ,",
"ja419:12.2": "na khippamanubujjhati;",
"ja419:12.3": "So haññati mandamati,",
"ja419:12.4": "corova girigabbhare.",
"ja419:13.1": "Yo ca uppatitaṃ atthaṃ,",
"ja419:13.2": "khippameva nibodhati;",
"ja419:13.3": "Muccate sattusambādhā,",
"ja419:13.4": "sulasā sattukāmivā”ti.",
"ja419:14.1": "Sulasājātakaṃ tatiyaṃ."
}