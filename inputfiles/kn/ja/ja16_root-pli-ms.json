{
"ja16:1.1": "Jātaka",
"ja16:2.1": "Ekakanipāta",
"ja16:3.1": "Sīlavagga",
"ja16:4.1": "6. Tipallatthamigajātaka",
"ja16:5.1": "“Migaṃ tipallatthamanekamāyaṃ,",
"ja16:5.2": "Aṭṭhakkhuraṃ aḍḍharattāpapāyiṃ;",
"ja16:5.3": "Ekena sotena chamāssasanto,",
"ja16:5.4": "Chahi kalāhitibhoti bhāgineyyo”ti.",
"ja16:6.1": "Tipallatthamigajātakaṃ chaṭṭhaṃ."
}