{
"ja260:1.1": "Jātaka",
"ja260:2.1": "Tikanipāta",
"ja260:3.1": "Saṅkappavagga",
"ja260:4.1": "10. Dūtajātaka",
"ja260:5.1": "“Yassatthā dūramāyanti,",
"ja260:5.2": "amittamapi yācituṃ;",
"ja260:5.3": "Tassūdarassahaṃ dūto,",
"ja260:5.4": "mā me kujjha rathesabha”.",
"ja260:6.1": "“Yassa divā ca ratto ca,",
"ja260:6.2": "vasamāyanti māṇavā;",
"ja260:6.3": "Tassūdarassahaṃ dūto,",
"ja260:6.4": "mā me kujjha rathesabha.",
"ja260:7.1": "Dadāmi te brāhmaṇa rohiṇīnaṃ,",
"ja260:7.2": "Gavaṃ sahassaṃ saha puṅgavena;",
"ja260:7.3": "Dūto hi dūtassa kathaṃ na dajjaṃ,",
"ja260:7.4": "Mayampi tasseva bhavāma dūtā”ti.",
"ja260:8.1": "Dūtajātakaṃ dasamaṃ.",
"ja260:8.2": "Saṅkappavaggo paṭhamo.",
"ja260:9.1": "Tassuddānaṃ",
"ja260:10.1": "Usukāravaro tilamuṭṭhi maṇi,",
"ja260:10.2": "Hayarāja vihaṅgama āsiviso;",
"ja260:10.3": "Janasandha kahāpaṇavassa puna,",
"ja260:10.4": "Tiriṭaṃ puna dūtavarena dasāti."
}