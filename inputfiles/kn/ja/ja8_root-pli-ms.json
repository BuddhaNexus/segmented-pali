{
  "ja8:0.1": "Jātaka ",
  "ja8:0.2": "Ekakanipāta ",
  "ja8:0.3": "Apaṇṇakavagga ",
  "ja8:0.4": "8. Gāmaṇijātaka ",
  "ja8:1.1": "“Api ataramānānaṁ, ",
  "ja8:1.2": "phalāsāva samijjhati; ",
  "ja8:1.3": "Vipakkabrahmacariyosmi, ",
  "ja8:1.4": "evaṁ jānāhi gāmaṇī”ti. ",
  "ja8:2.1": "Gāmaṇijātakaṁ aṭṭhamaṁ. "
}