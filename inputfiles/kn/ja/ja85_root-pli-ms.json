{
"ja85:1.1": "Jātaka",
"ja85:2.1": "Ekakanipāta",
"ja85:3.1": "Apāyimhavagga",
"ja85:4.1": "5. Kiṃpakkajātaka",
"ja85:5.1": "“Āyatiṃ dosaṃ nāññāya,",
"ja85:5.2": "yo kāme paṭisevati;",
"ja85:5.3": "Vipākante hananti naṃ,",
"ja85:5.4": "kiṃpakkamiva bhakkhitan”ti.",
"ja85:6.1": "Kiṃpakkajātakaṃ pañcamaṃ."
}