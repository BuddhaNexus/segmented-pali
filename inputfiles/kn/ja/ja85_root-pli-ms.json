{
  "ja85:0.1": "Jātaka ",
  "ja85:0.2": "Ekakanipāta ",
  "ja85:0.3": "Apāyimhavagga ",
  "ja85:0.4": "5. Kiṁpakkajātaka ",
  "ja85:1.1": "“Āyatiṁ dosaṁ nāññāya, ",
  "ja85:1.2": "yo kāme paṭisevati; ",
  "ja85:1.3": "Vipākante hananti naṁ, ",
  "ja85:1.4": "kiṁpakkamiva bhakkhitan”ti. ",
  "ja85:2.1": "Kiṁpakkajātakaṁ pañcamaṁ. "
}