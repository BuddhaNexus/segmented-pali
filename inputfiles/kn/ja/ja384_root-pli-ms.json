{
"ja384:1.1": "Jātaka",
"ja384:2.1": "Chakkanipāta",
"ja384:3.1": "Avāriyavagga",
"ja384:4.1": "9. Dhammadhajajātaka",
"ja384:5.1": "“Dhammaṃ caratha ñātayo,",
"ja384:5.2": "Dhammaṃ caratha bhaddaṃ vo;",
"ja384:5.3": "Dhammacārī sukhaṃ seti,",
"ja384:5.4": "Asmiṃ loke paramhi ca”.",
"ja384:6.1": "“Bhaddako vatayaṃ pakkhī,",
"ja384:6.2": "dijo paramadhammiko;",
"ja384:6.3": "Ekapādena tiṭṭhanto,",
"ja384:6.4": "dhammamevānusāsati”.",
"ja384:7.1": "“Nāssa sīlaṃ vijānātha,",
"ja384:7.2": "anaññāya pasaṃsatha;",
"ja384:7.3": "Bhutvā aṇḍañca potañca,",
"ja384:7.4": "dhammo dhammoti bhāsati.",
"ja384:8.1": "Aññaṃ bhaṇati vācāya,",
"ja384:8.2": "aññaṃ kāyena kubbati;",
"ja384:8.3": "Vācāya no ca kāyena,",
"ja384:8.4": "na taṃ dhammaṃ adhiṭṭhito.",
"ja384:9.1": "Vācāya sakhilo manoviduggo,",
"ja384:9.2": "Channo kūpasayova kaṇhasappo;",
"ja384:9.3": "Dhammadhajo gāmanigamāsu sādhu,",
"ja384:9.4": "Dujjāno purisena bālisena.",
"ja384:10.1": "Imaṃ tuṇḍehi pakkhehi,",
"ja384:10.2": "pādā cimaṃ viheṭhatha;",
"ja384:10.3": "Chavañhimaṃ vināsetha,",
"ja384:10.4": "nāyaṃ saṃvāsanāraho”ti.",
"ja384:11.1": "Dhammadhajajātakaṃ navamaṃ."
}