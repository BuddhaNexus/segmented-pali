{
"ja409:1.1": "Jātaka",
"ja409:2.1": "Sattakanipāta",
"ja409:3.1": "Gandhāravagga",
"ja409:4.1": "4. Daḷhadhammajātaka",
"ja409:5.1": "“Ahañce daḷhadhammassa,",
"ja409:5.2": "vahantī nābhirādhayiṃ;",
"ja409:5.3": "Dharantī urasi sallaṃ,",
"ja409:5.4": "yuddhe vikkantacārinī.",
"ja409:6.1": "Nūna rājā na jānāti,",
"ja409:6.2": "mama vikkamaporisaṃ;",
"ja409:6.3": "Saṅgāme sukatantāni,",
"ja409:6.4": "dūtavippahitāni ca.",
"ja409:7.1": "Sā nūnāhaṃ marissāmi,",
"ja409:7.2": "abandhu aparāyinī;",
"ja409:7.3": "Tadā hi kumbhakārassa,",
"ja409:7.4": "dinnā chakaṇahārikā”.",
"ja409:8.1": "“Yāvatāsīsatī poso,",
"ja409:8.2": "tāvadeva pavīṇati;",
"ja409:8.3": "Atthāpāye jahanti naṃ,",
"ja409:8.4": "oṭṭhibyādhiṃva khattiyo.",
"ja409:9.1": "Yo pubbe katakalyāṇo,",
"ja409:9.2": "katattho nāvabujjhati;",
"ja409:9.3": "Atthā tassa palujjanti,",
"ja409:9.4": "ye honti abhipatthitā.",
"ja409:10.1": "Yo pubbe katakalyāṇo,",
"ja409:10.2": "katattho manubujjhati;",
"ja409:10.3": "Atthā tassa pavaḍḍhanti,",
"ja409:10.4": "ye honti abhipatthitā.",
"ja409:11.1": "Taṃ vo vadāmi bhaddante,",
"ja409:11.2": "yāvantettha samāgatā;",
"ja409:11.3": "Sabbe kataññuno hotha,",
"ja409:11.4": "ciraṃ saggamhi ṭhassathā”ti.",
"ja409:12.1": "Daḷhadhammajātakaṃ catutthaṃ."
}