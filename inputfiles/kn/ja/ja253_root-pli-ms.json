{
"ja253:1.1": "Jātaka",
"ja253:2.1": "Tikanipāta",
"ja253:3.1": "Saṅkappavagga",
"ja253:4.1": "3. Maṇikaṇṭhajātaka",
"ja253:5.1": "“Mamannapānaṃ vipulaṃ uḷāraṃ,",
"ja253:5.2": "Uppajjatīmassa maṇissa hetu;",
"ja253:5.3": "Taṃ te na dassaṃ atiyācakosi,",
"ja253:5.4": "Na cāpi te assamamāgamissaṃ.",
"ja253:6.1": "Susū yathā sakkharadhotapāṇī,",
"ja253:6.2": "Tāsesi maṃ selaṃ yācamāno;",
"ja253:6.3": "Taṃ te na dassaṃ atiyācakosi,",
"ja253:6.4": "Na cāpi te assamamāgamissaṃ”.",
"ja253:7.1": "“Na taṃ yāce yassa piyaṃ jigīse,",
"ja253:7.2": "Desso hoti atiyācanāya;",
"ja253:7.3": "Nāgo maṇiṃ yācito brāhmaṇena,",
"ja253:7.4": "Adassanaṃyeva tadajjhagamā”ti.",
"ja253:8.1": "Maṇikaṇṭhajātakaṃ tatiyaṃ."
}