{
  "ja414:0.1": "Jātaka ",
  "ja414:0.2": "Sattakanipāta ",
  "ja414:0.3": "Gandhāravagga ",
  "ja414:0.4": "9. Jāgarajātaka ",
  "ja414:1.1": "“Kodha jāgarataṁ sutto, ",
  "ja414:1.2": "kodha suttesu jāgaro; ",
  "ja414:1.3": "Ko mametaṁ vijānāti, ",
  "ja414:1.4": "ko taṁ paṭibhaṇāti me”. ",
  "ja414:2.1": "“Ahaṁ jāgarataṁ sutto, ",
  "ja414:2.2": "ahaṁ suttesu jāgaro; ",
  "ja414:2.3": "Ahametaṁ vijānāmi, ",
  "ja414:2.4": "ahaṁ paṭibhaṇāmi te”. ",
  "ja414:3.1": "“Kathaṁ jāgarataṁ sutto, ",
  "ja414:3.2": "kathaṁ suttesu jāgaro; ",
  "ja414:3.3": "Kathaṁ etaṁ vijānāsi, ",
  "ja414:3.4": "kathaṁ paṭibhaṇāsi me”. ",
  "ja414:4.1": "“Ye dhammaṁ nappajānanti, ",
  "ja414:4.2": "saṁyamoti damoti ca; ",
  "ja414:4.3": "Tesu suppamānesu, ",
  "ja414:4.4": "ahaṁ jaggāmi devate. ",
  "ja414:5.1": "Yesaṁ rāgo ca doso ca, ",
  "ja414:5.2": "avijjā ca virājitā; ",
  "ja414:5.3": "Tesu jāgaramānesu, ",
  "ja414:5.4": "ahaṁ suttosmi devate. ",
  "ja414:6.1": "Evaṁ jāgarataṁ sutto, ",
  "ja414:6.2": "evaṁ suttesu jāgaro; ",
  "ja414:6.3": "Evametaṁ vijānāmi, ",
  "ja414:6.4": "evaṁ paṭibhaṇāmi te”. ",
  "ja414:7.1": "“Sādhu jāgarataṁ sutto, ",
  "ja414:7.2": "sādhu suttesu jāgaro; ",
  "ja414:7.3": "Sādhumetaṁ vijānāsi, ",
  "ja414:7.4": "sādhu paṭibhaṇāsi me”ti. ",
  "ja414:8.1": "Jāgarajātakaṁ navamaṁ. "
}