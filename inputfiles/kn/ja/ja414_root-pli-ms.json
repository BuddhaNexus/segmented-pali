{
"ja414:1.1": "Jātaka",
"ja414:2.1": "Sattakanipāta",
"ja414:3.1": "Gandhāravagga",
"ja414:4.1": "9. Jāgarajātaka",
"ja414:5.1": "“Kodha jāgarataṃ sutto,",
"ja414:5.2": "kodha suttesu jāgaro;",
"ja414:5.3": "Ko mametaṃ vijānāti,",
"ja414:5.4": "ko taṃ paṭibhaṇāti me”.",
"ja414:6.1": "“Ahaṃ jāgarataṃ sutto,",
"ja414:6.2": "ahaṃ suttesu jāgaro;",
"ja414:6.3": "Ahametaṃ vijānāmi,",
"ja414:6.4": "ahaṃ paṭibhaṇāmi te”.",
"ja414:7.1": "“Kathaṃ jāgarataṃ sutto,",
"ja414:7.2": "kathaṃ suttesu jāgaro;",
"ja414:7.3": "Kathaṃ etaṃ vijānāsi,",
"ja414:7.4": "kathaṃ paṭibhaṇāsi me”.",
"ja414:8.1": "“Ye dhammaṃ nappajānanti,",
"ja414:8.2": "saṃyamoti damoti ca;",
"ja414:8.3": "Tesu suppamānesu,",
"ja414:8.4": "ahaṃ jaggāmi devate.",
"ja414:9.1": "Yesaṃ rāgo ca doso ca,",
"ja414:9.2": "avijjā ca virājitā;",
"ja414:9.3": "Tesu jāgaramānesu,",
"ja414:9.4": "ahaṃ suttosmi devate.",
"ja414:10.1": "Evaṃ jāgarataṃ sutto,",
"ja414:10.2": "evaṃ suttesu jāgaro;",
"ja414:10.3": "Evametaṃ vijānāmi,",
"ja414:10.4": "evaṃ paṭibhaṇāmi te”.",
"ja414:11.1": "“Sādhu jāgarataṃ sutto,",
"ja414:11.2": "sādhu suttesu jāgaro;",
"ja414:11.3": "Sādhumetaṃ vijānāsi,",
"ja414:11.4": "sādhu paṭibhaṇāsi me”ti.",
"ja414:12.1": "Jāgarajātakaṃ navamaṃ."
}