{
"ja502:1.1": "Jātaka",
"ja502:2.1": "Vīsatinipāta",
"ja502:3.1": "Mātaṅgavagga",
"ja502:4.1": "6. Cūḷahaṃsajātaka",
"ja502:5.1": "“Ete haṃsā pakkamanti,",
"ja502:5.2": "vakkaṅgā bhayameritā;",
"ja502:5.3": "Harittaca hemavaṇṇa,",
"ja502:5.4": "kāmaṃ sumukha pakkama.",
"ja502:6.1": "Ohāya maṃ ñātigaṇā,",
"ja502:6.2": "ekaṃ pāsavasaṃ gataṃ;",
"ja502:6.3": "Anapekkhamānā gacchanti,",
"ja502:6.4": "kiṃ eko avahiyyasi.",
"ja502:7.1": "Pateva patataṃ seṭṭha,",
"ja502:7.2": "natthi baddhe sahāyatā;",
"ja502:7.3": "Mā anīghāya hāpesi,",
"ja502:7.4": "kāmaṃ sumukha pakkama”.",
"ja502:8.1": "“Nāhaṃ ‘dukkhapareto’ti,",
"ja502:8.2": "dhataraṭṭha tuvaṃ jahe;",
"ja502:8.3": "Jīvitaṃ maraṇaṃ vā me,",
"ja502:8.4": "tayā saddhiṃ bhavissati”.",
"ja502:9.1": "“Etadariyassa kalyāṇaṃ,",
"ja502:9.2": "yaṃ tvaṃ sumukha bhāsasi;",
"ja502:9.3": "Tañca vīmaṃsamānohaṃ,",
"ja502:9.4": "‘patatetaṃ’ avassajiṃ”.",
"ja502:10.1": "“Apadena padaṃ yāti,",
"ja502:10.2": "antalikkhacaro dijo;",
"ja502:10.3": "Ārā pāsaṃ na bujjhi tvaṃ,",
"ja502:10.4": "haṃsānaṃ pavaruttama”.",
"ja502:11.1": "“Yadā parābhavo hoti,",
"ja502:11.2": "poso jīvitasaṅkhaye;",
"ja502:11.3": "Atha jālañca pāsañca,",
"ja502:11.4": "āsajjāpi na bujjhati”.",
"ja502:12.1": "“Ete haṃsā pakkamanti,",
"ja502:12.2": "vakkaṅgā bhayameritā;",
"ja502:12.3": "Harittaca hemavaṇṇa,",
"ja502:12.4": "tvaññeva avahiyyasi.",
"ja502:13.1": "Ete bhutvā ca pitvā ca,",
"ja502:13.2": "pakkamanti vihaṅgamā;",
"ja502:13.3": "Anapekkhamānā vakkaṅgā,",
"ja502:13.4": "tvaññeveko upāsasi.",
"ja502:14.1": "Kiṃ nu tyāyaṃ dijo hoti,",
"ja502:14.2": "mutto baddhaṃ upāsasi;",
"ja502:14.3": "Ohāya sakuṇā yanti,",
"ja502:14.4": "kiṃ eko avahiyyasi”.",
"ja502:15.1": "“Rājā me so dijo mitto,",
"ja502:15.2": "sakhā pāṇasamo ca me;",
"ja502:15.3": "Neva naṃ vijahissāmi,",
"ja502:15.4": "yāva kālassa pariyāyaṃ”.",
"ja502:16.1": "“Yo ca tvaṃ sakhino hetu,",
"ja502:16.2": "pāṇaṃ cajitumicchasi;",
"ja502:16.3": "So te sahāyaṃ muñcāmi,",
"ja502:16.4": "hotu rājā tavānugo”.",
"ja502:17.1": "“Evaṃ luddaka nandassu,",
"ja502:17.2": "saha sabbehi ñātibhi;",
"ja502:17.3": "Yathāhamajja nandāmi,",
"ja502:17.4": "muttaṃ disvā dijādhipaṃ”.",
"ja502:18.1": "“Kaccinnu bhoto kusalaṃ,",
"ja502:18.2": "kacci bhoto anāmayaṃ;",
"ja502:18.3": "Kacci raṭṭhamidaṃ phītaṃ,",
"ja502:18.4": "dhammena manusāsasi”.",
"ja502:19.1": "“Kusalañceva me haṃsa,",
"ja502:19.2": "atho haṃsa anāmayaṃ;",
"ja502:19.3": "Atho raṭṭhamidaṃ phītaṃ,",
"ja502:19.4": "dhammena manusāsahaṃ”.",
"ja502:20.1": "“Kacci bhoto amaccesu,",
"ja502:20.2": "doso koci na vijjati;",
"ja502:20.3": "Kacci ārā amittā te,",
"ja502:20.4": "chāyā dakkhiṇatoriva”.",
"ja502:21.1": "“Athopi me amaccesu,",
"ja502:21.2": "doso koci na vijjati;",
"ja502:21.3": "Atho ārā amittā me,",
"ja502:21.4": "chāyā dakkhiṇatoriva”.",
"ja502:22.1": "“Kacci te sādisī bhariyā,",
"ja502:22.2": "assavā piyabhāṇinī;",
"ja502:22.3": "Puttarūpayasūpetā,",
"ja502:22.4": "tava chandavasānugā”.",
"ja502:23.1": "“Atho me sādisī bhariyā,",
"ja502:23.2": "assavā piyabhāṇinī;",
"ja502:23.3": "Puttarūpayasūpetā,",
"ja502:23.4": "mama chandavasānugā”.",
"ja502:24.1": "“Kacci te bahavo puttā,",
"ja502:24.2": "sujātā raṭṭhavaḍḍhana;",
"ja502:24.3": "Paññājavena sampannā,",
"ja502:24.4": "sammodanti tato tato”.",
"ja502:25.1": "“Satameko ca me puttā,",
"ja502:25.2": "dhataraṭṭha mayā sutā;",
"ja502:25.3": "Tesaṃ tvaṃ kiccamakkhāhi,",
"ja502:25.4": "nāvarujjhanti te vaco”.",
"ja502:26.1": "“Upapannopi ce hoti,",
"ja502:26.2": "jātiyā vinayena vā;",
"ja502:26.3": "Atha pacchā kurute yogaṃ,",
"ja502:26.4": "kicche āpāsu sīdati.",
"ja502:27.1": "Tassa saṃhīrapaññassa,",
"ja502:27.2": "vivaro jāyate mahā;",
"ja502:27.3": "Rattimandhova rūpāni,",
"ja502:27.4": "thūlāni manupassati.",
"ja502:28.1": "Asāre sārayogaññū,",
"ja502:28.2": "matiṃ na tveva vindati;",
"ja502:28.3": "Sarabhova giriduggasmiṃ,",
"ja502:28.4": "antarāyeva sīdati.",
"ja502:29.1": "Hīnajaccopi ce hoti,",
"ja502:29.2": "uṭṭhātā dhitimā naro;",
"ja502:29.3": "Ācārasīlasampanno,",
"ja502:29.4": "nise aggīva bhāsati.",
"ja502:30.1": "Etaṃ me upamaṃ katvā,",
"ja502:30.2": "putte vijjāsu vācaya;",
"ja502:30.3": "Saṃvirūḷhetha medhāvī,",
"ja502:30.4": "khette bījaṃva vuṭṭhiyā”ti.",
"ja502:31.1": "Cūḷahaṃsajātakaṃ chaṭṭhaṃ."
}