{
  "ja30:0.1": "Jātaka ",
  "ja30:0.2": "Ekakanipāta ",
  "ja30:0.3": "Kuruṅgavagga ",
  "ja30:0.4": "10. Munikajātaka ",
  "ja30:1.1": "“Mā munikassa pihayi, ",
  "ja30:1.2": "āturannāni bhuñjati; ",
  "ja30:1.3": "Appossukko bhusaṁ khāda, ",
  "ja30:1.4": "etaṁ dīghāyulakkhaṇan”ti. ",
  "ja30:2.1": "Munikajātakaṁ dasamaṁ. ",
  "ja30:2.2": "Kuruṅgavaggo tatiyo. ",
  "ja30:3.1": "Tassuddānaṁ ",
  "ja30:4.1": "Kuruṅgassa kukkuragojavaro, ",
  "ja30:4.2": "Puna vāḷavassasirivhayano; ",
  "ja30:4.3": "Mahiḷāmukhanāmanuññavaro, ",
  "ja30:4.4": "Vahate dhura munikena dasāti. "
}