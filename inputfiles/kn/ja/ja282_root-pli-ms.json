{
"ja282:1.1": "Jātaka",
"ja282:2.1": "Tikanipāta",
"ja282:3.1": "Abbhantaravagga",
"ja282:4.1": "2. Seyyajātaka",
"ja282:5.1": "“Seyyaṃso seyyaso hoti,",
"ja282:5.2": "yo seyyamupasevati;",
"ja282:5.3": "Ekena sandhiṃ katvāna,",
"ja282:5.4": "sataṃ vajjhe amocayiṃ.",
"ja282:6.1": "Tasmā sabbena lokena,",
"ja282:6.2": "sandhiṃ katvāna ekato;",
"ja282:6.3": "Pecca saggaṃ nigaccheyya,",
"ja282:6.4": "idaṃ suṇātha kāsiyā”.",
"ja282:7.1": "Idaṃ vatvā mahārājā,",
"ja282:7.2": "Kaṃso bārāṇasiggaho;",
"ja282:7.3": "Dhanuṃ kaṇḍañca nikkhippa,",
"ja282:7.4": "Saṃyamaṃ ajjhupāgamīti.",
"ja282:8.1": "Seyyajātakaṃ dutiyaṃ."
}