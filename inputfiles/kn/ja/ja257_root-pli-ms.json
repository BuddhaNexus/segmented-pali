{
"ja257:1.1": "Jātaka",
"ja257:2.1": "Tikanipāta",
"ja257:3.1": "Saṅkappavagga",
"ja257:4.1": "7. Gāmaṇicandajātaka",
"ja257:5.1": "“Nāyaṃ gharānaṃ kusalo,",
"ja257:5.2": "lolo ayaṃ valīmukho;",
"ja257:5.3": "Kataṃ kataṃ kho dūseyya,",
"ja257:5.4": "evaṃdhammamidaṃ kulaṃ.",
"ja257:6.1": "Nayidaṃ cittavato lomaṃ,",
"ja257:6.2": "nāyaṃ assāsiko migo;",
"ja257:6.3": "Siṭṭhaṃ me janasandhena,",
"ja257:6.4": "nāyaṃ kiñci vijānati.",
"ja257:7.1": "Na mātaraṃ pitaraṃ vā,",
"ja257:7.2": "bhātaraṃ bhaginiṃ sakaṃ;",
"ja257:7.3": "Bhareyya tādiso poso,",
"ja257:7.4": "siṭṭhaṃ dasarathena me”ti.",
"ja257:8.1": "Gāmaṇicanda jātakaṃ sattamaṃ."
}