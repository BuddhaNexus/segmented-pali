{
"ja101:1.1": "Jātaka",
"ja101:2.1": "Ekakanipāta",
"ja101:3.1": "Parosatavagga",
"ja101:4.1": "1. Parosatajātaka",
"ja101:5.1": "“Parosatañcepi samāgatānaṃ,",
"ja101:5.2": "Jhāyeyyuṃ te vassasataṃ apaññā;",
"ja101:5.3": "Ekova seyyo puriso sapañño,",
"ja101:5.4": "Yo bhāsitassa vijānāti atthan”ti.",
"ja101:6.1": "Parosatajātakaṃ paṭhamaṃ."
}