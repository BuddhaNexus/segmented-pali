{
"ja21:1.1": "Jātaka",
"ja21:2.1": "Ekakanipāta",
"ja21:3.1": "Kuruṅgavagga",
"ja21:4.1": "1. Kuruṅgamigajātaka",
"ja21:5.1": "“Ñātametaṃ kuruṅgassa,",
"ja21:5.2": "yaṃ tvaṃ sepaṇṇi siyyasi;",
"ja21:5.3": "Aññaṃ sepaṇṇi gacchāmi,",
"ja21:5.4": "na me te ruccate phalan”ti.",
"ja21:6.1": "Kuruṅgamigajātakaṃ paṭhamaṃ."
}