{
"ja127:1.1": "Jātaka",
"ja127:2.1": "Ekakanipāta",
"ja127:3.1": "Kusanāḷivagga",
"ja127:4.1": "7. Kalaṇḍukajātaka",
"ja127:5.1": "“Te desā tāni vatthūni,",
"ja127:5.2": "ahañca vanagocaro;",
"ja127:5.3": "Anuvicca kho taṃ gaṇheyyuṃ,",
"ja127:5.4": "piva khīraṃ kalaṇḍukā”ti.",
"ja127:6.1": "Kalaṇḍukajātakaṃ sattamaṃ."
}