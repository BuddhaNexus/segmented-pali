{
"ja14:1.1": "Jātaka",
"ja14:2.1": "Ekakanipāta",
"ja14:3.1": "Sīlavagga",
"ja14:4.1": "4. Vātamigajātaka",
"ja14:5.1": "“Na kiratthi rasehi pāpiyo,",
"ja14:5.2": "Āvāsehi va santhavehi vā;",
"ja14:5.3": "Vātamigaṃ gahananissitaṃ,",
"ja14:5.4": "Vasamānesi rasehi sañjayo”ti.",
"ja14:6.1": "Vātamigajātakaṃ catutthaṃ."
}