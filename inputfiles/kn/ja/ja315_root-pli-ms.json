{
"ja315:1.1": "Jātaka",
"ja315:2.1": "Catukkanipāta",
"ja315:3.1": "Pucimandavagga",
"ja315:4.1": "5. Sabbamaṃsalābhajātaka",
"ja315:5.1": "“Pharusā vata te vācā,",
"ja315:5.2": "maṃsaṃ yācanako asi;",
"ja315:5.3": "Kilomasadisī vācā,",
"ja315:5.4": "kilomaṃ samma dammi te”.",
"ja315:6.1": "“Aṅgametaṃ manussānaṃ,",
"ja315:6.2": "bhātā loke pavuccati;",
"ja315:6.3": "Aṅgassa sadisī vācā,",
"ja315:6.4": "aṅgaṃ samma dadāmi te”.",
"ja315:7.1": "“Tātāti putto vadamāno,",
"ja315:7.2": "kampeti hadayaṃ pitu;",
"ja315:7.3": "Hadayassa sadisī vācā,",
"ja315:7.4": "hadayaṃ samma dammi te”.",
"ja315:8.1": "“Yassa gāme sakhā natthi,",
"ja315:8.2": "yathāraññaṃ tatheva taṃ;",
"ja315:8.3": "Sabbassa sadisī vācā,",
"ja315:8.4": "sabbaṃ samma dadāmi te”ti.",
"ja315:9.1": "Sabbamaṃsalābhajātakaṃ pañcamaṃ."
}