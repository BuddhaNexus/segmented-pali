{
"ja308:1.1": "Jātaka",
"ja308:2.1": "Catukkanipāta",
"ja308:3.1": "Kāliṅgavagga",
"ja308:4.1": "8. Sakuṇajātaka",
"ja308:5.1": "“Akaramhasa te kiccaṃ,",
"ja308:5.2": "yaṃ balaṃ ahuvamhase;",
"ja308:5.3": "Migarāja namo tyatthu,",
"ja308:5.4": "api kiñci labhāmase”.",
"ja308:6.1": "“Mama lohitabhakkhassa,",
"ja308:6.2": "niccaṃ luddāni kubbato;",
"ja308:6.3": "Dantantaragato santo,",
"ja308:6.4": "taṃ bahuṃ yampi jīvasi”.",
"ja308:7.1": "“Akataññumakattāraṃ,",
"ja308:7.2": "katassa appaṭikārakaṃ;",
"ja308:7.3": "Yasmiṃ kataññutā natthi,",
"ja308:7.4": "niratthā tassa sevanā.",
"ja308:8.1": "Yassa sammukhaciṇṇena,",
"ja308:8.2": "Mittadhammo na labbhati;",
"ja308:8.3": "Anusūyamanakkosaṃ,",
"ja308:8.4": "Saṇikaṃ tamhā apakkame”ti.",
"ja308:9.1": "Sakuṇajātakaṃ aṭṭhamaṃ."
}