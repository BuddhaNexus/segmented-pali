{
"ja244:1.1": "Jātaka",
"ja244:2.1": "Dukanipāta",
"ja244:3.1": "Siṅgālavagga",
"ja244:4.1": "4. Vigaticchajātaka",
"ja244:5.1": "“Yaṃ passati na taṃ icchati,",
"ja244:5.2": "Yañca na passati taṃ kiricchati;",
"ja244:5.3": "Maññāmi ciraṃ carissati,",
"ja244:5.4": "Na hi taṃ lacchati yaṃ sa icchati.",
"ja244:6.1": "Yaṃ labhati na tena tussati,",
"ja244:6.2": "Yañca pattheti laddhaṃ hīḷeti;",
"ja244:6.3": "Icchā hi anantagocarā,",
"ja244:6.4": "Vigaticchāna namo karomase”ti.",
"ja244:7.1": "Vigaticchajātakaṃ catutthaṃ."
}