{
"ja249:1.1": "Jātaka",
"ja249:2.1": "Dukanipāta",
"ja249:3.1": "Siṅgālavagga",
"ja249:4.1": "9. Sālakajātaka",
"ja249:5.1": "“Ekaputtako bhavissasi,",
"ja249:5.2": "Tvañca no hessasi issaro kule;",
"ja249:5.3": "Oroha dumasmā sālaka,",
"ja249:5.4": "Ehi dāni gharakaṃ vajemase”.",
"ja249:6.1": "“Nanu maṃ suhadayoti maññasi,",
"ja249:6.2": "Yañca maṃ hanasi veḷuyaṭṭhiyā;",
"ja249:6.3": "Pakkambavane ramāmase,",
"ja249:6.4": "Gaccha tvaṃ gharakaṃ yathāsukhan”ti.",
"ja249:7.1": "Sālakajātakaṃ navamaṃ."
}