{
  "ja476:0.1": "Jātaka ",
  "ja476:0.2": "Terasakanipāta ",
  "ja476:0.3": "Ambavagga ",
  "ja476:0.4": "3. Javanahaṁsajātaka ",
  "ja476:1.1": "“Idheva haṁsa nipata, ",
  "ja476:1.2": "piyaṁ me tava dassanaṁ; ",
  "ja476:1.3": "Issarosi anuppatto, ",
  "ja476:1.4": "yamidhatthi pavedaya”. ",
  "ja476:2.1": "“Savanena ekassa piyā bhavanti, ",
  "ja476:2.2": "Disvā panekassa viyeti chando; ",
  "ja476:2.3": "Disvā ca sutvā ca piyā bhavanti, ",
  "ja476:2.4": "Kaccinnu me pīyasi dassanena. ",
  "ja476:3.1": "Savanena piyo mesi, ",
  "ja476:3.2": "bhiyyo cāgamma dassanaṁ; ",
  "ja476:3.3": "Evaṁ piyadassano me, ",
  "ja476:3.4": "vasa haṁsa mamantike”. ",
  "ja476:4.1": "“Vaseyyāma tavāgāre, ",
  "ja476:4.2": "niccaṁ sakkatapūjitā; ",
  "ja476:4.3": "Matto ca ekadā vajje, ",
  "ja476:4.4": "‘haṁsarājaṁ pacantu me’”. ",
  "ja476:5.1": "“Dhiratthu taṁ majjapānaṁ, ",
  "ja476:5.2": "yaṁ me piyataraṁ tayā; ",
  "ja476:5.3": "Na cāpi majjaṁ pissāmi, ",
  "ja476:5.4": "yāva me vacchasī ghare”. ",
  "ja476:6.1": "“Suvijānaṁ siṅgālānaṁ, ",
  "ja476:6.2": "sakuṇānañca vassitaṁ; ",
  "ja476:6.3": "Manussavassitaṁ rāja, ",
  "ja476:6.4": "dubbijānataraṁ tato. ",
  "ja476:7.1": "Api ce maññatī poso, ",
  "ja476:7.2": "ñāti mitto sakhāti vā; ",
  "ja476:7.3": "Yo pubbe sumano hutvā, ",
  "ja476:7.4": "pacchā sampajjate diso. ",
  "ja476:8.1": "Yasmiṁ mano nivisati, ",
  "ja476:8.2": "avidūre sahāpi so; ",
  "ja476:8.3": "Santikepi hi so dūre, ",
  "ja476:8.4": "yasmiṁ nāvisate mano. ",
  "ja476:9.1": "Antopi so hoti pasannacitto, ",
  "ja476:9.2": "Pāraṁ samuddassa pasannacitto; ",
  "ja476:9.3": "Antopi so hoti paduṭṭhacitto, ",
  "ja476:9.4": "Pāraṁ samuddassa paduṭṭhacitto. ",
  "ja476:10.1": "Saṁvasantā vivasanti, ",
  "ja476:10.2": "ye disā te rathesabha; ",
  "ja476:10.3": "Ārā santo saṁvasanti, ",
  "ja476:10.4": "manasā raṭṭhavaḍḍhana. ",
  "ja476:11.1": "Aticiraṁ nivāsena, ",
  "ja476:11.2": "piyo bhavati appiyo; ",
  "ja476:11.3": "Āmanta kho taṁ gacchāma, ",
  "ja476:11.4": "purā te homa appiyā”. ",
  "ja476:12.1": "“Evañce yācamānānaṁ, ",
  "ja476:12.2": "Añjaliṁ nāvabujjhasi; ",
  "ja476:12.3": "Paricārakānaṁ sataṁ, ",
  "ja476:12.4": "Vacanaṁ na karosi no; ",
  "ja476:12.5": "Evaṁ taṁ abhiyācāma, ",
  "ja476:12.6": "Puna kayirāsi pariyāyaṁ”. ",
  "ja476:13.1": "“Evañce no viharataṁ, ",
  "ja476:13.2": "antarāyo na hessati; ",
  "ja476:13.3": "Tuyhañcāpi mahārāja, ",
  "ja476:13.4": "mayhañca raṭṭhavaḍḍhana; ",
  "ja476:13.5": "Appeva nāma passemu, ",
  "ja476:13.6": "ahorattānamaccaye”ti. ",
  "ja476:14.1": "Javanahaṁsajātakaṁ tatiyaṁ. "
}