{
  "ja173:0.1": "Jātaka ",
  "ja173:0.2": "Dukanipāta ",
  "ja173:0.3": "Kalyāṇavagga ",
  "ja173:0.4": "3. Makkaṭajātaka ",
  "ja173:1.1": "“Tāta māṇavako eso, ",
  "ja173:1.2": "tālamūlaṁ apassito; ",
  "ja173:1.3": "Agārakañcidaṁ atthi, ",
  "ja173:1.4": "handa demassagārakaṁ”. ",
  "ja173:2.1": "“Mā kho tvaṁ tāta pakkosi, ",
  "ja173:2.2": "dūseyya no agārakaṁ; ",
  "ja173:2.3": "Netādisaṁ mukhaṁ hoti, ",
  "ja173:2.4": "brāhmaṇassa susīlino”ti. ",
  "ja173:3.1": "Makkaṭajātakaṁ tatiyaṁ. "
}