{
"ja345:1.1": "Jātaka",
"ja345:2.1": "Catukkanipāta",
"ja345:3.1": "Cūḷakuṇālavagga",
"ja345:4.1": "5. Gajakumbhajātaka",
"ja345:5.1": "“Vanaṃ yadaggi dahati,",
"ja345:5.2": "pāvako kaṇhavattanī;",
"ja345:5.3": "Kathaṃ karosi pacalaka,",
"ja345:5.4": "evaṃ dandhaparakkamo”.",
"ja345:6.1": "“Bahūni rukkhachiddāni,",
"ja345:6.2": "pathabyā vivarāni ca;",
"ja345:6.3": "Tāni ce nābhisambhoma,",
"ja345:6.4": "hoti no kālapariyāyo”.",
"ja345:7.1": "“Yo dandhakāle tarati,",
"ja345:7.2": "taraṇīye ca dandhati;",
"ja345:7.3": "Sukkhapaṇṇaṃva akkamma,",
"ja345:7.4": "atthaṃ bhañjati attano.",
"ja345:8.1": "Yo dandhakāle dandheti,",
"ja345:8.2": "taraṇīye ca tārayi;",
"ja345:8.3": "Sasīva rattiṃ vibhajaṃ,",
"ja345:8.4": "tassattho paripūratī”ti.",
"ja345:9.1": "Gajakumbhajātakaṃ pañcamaṃ."
}