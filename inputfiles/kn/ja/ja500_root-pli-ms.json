{
"ja500:1.1": "Jātaka",
"ja500:2.1": "Vīsatinipāta",
"ja500:3.1": "Mātaṅgavagga",
"ja500:4.1": "4. Sirīmantajātaka",
"ja500:5.1": "“Paññāyupetaṃ siriyā vihīnaṃ,",
"ja500:5.2": "Yasassinaṃ vāpi apetapaññaṃ;",
"ja500:5.3": "Pucchāmi taṃ senaka etamatthaṃ,",
"ja500:5.4": "Kamettha seyyo kusalā vadanti”.",
"ja500:6.1": "“Dhīrā ca bālā ca have janinda,",
"ja500:6.2": "Sippūpapannā ca asippino ca;",
"ja500:6.3": "Sujātimantopi ajātimassa,",
"ja500:6.4": "Yasassino pesakarā bhavanti;",
"ja500:6.5": "Etampi disvāna ahaṃ vadāmi,",
"ja500:6.6": "Pañño nihīno sirīmāva seyyo”.",
"ja500:7.1": "“Tuvampi pucchāmi anomapañña,",
"ja500:7.2": "Mahosadha kevaladhammadassi;",
"ja500:7.3": "Bālaṃ yasassiṃ paṇḍitaṃ appabhogaṃ,",
"ja500:7.4": "Kamettha seyyo kusalā vadanti”.",
"ja500:8.1": "“Pāpāni kammāni karoti bālo,",
"ja500:8.2": "Idhameva seyyo iti maññamāno;",
"ja500:8.3": "Idhalokadassī paralokamadassī,",
"ja500:8.4": "Ubhayattha bālo kalimaggahesi;",
"ja500:8.5": "Etampi disvāna ahaṃ vadāmi,",
"ja500:8.6": "Paññova seyyo na yasassi bālo”.",
"ja500:9.1": "“Na sippametaṃ vidadhāti bhogaṃ,",
"ja500:9.2": "Na bandhuvā na sarīravaṇṇo yo;",
"ja500:9.3": "Passeḷamūgaṃ sukhamedhamānaṃ,",
"ja500:9.4": "Sirī hi naṃ bhajate goravindaṃ;",
"ja500:9.5": "Etampi disvāna ahaṃ vadāmi,",
"ja500:9.6": "Pañño nihīno sirīmāva seyyo”.",
"ja500:10.1": "“Laddhā sukhaṃ majjati appapañño,",
"ja500:10.2": "Dukkhena phuṭṭhopi pamohameti;",
"ja500:10.3": "Āgantunā dukkhasukhena phuṭṭho,",
"ja500:10.4": "Pavedhati vāricarova ghamme;",
"ja500:10.5": "Etampi disvāna ahaṃ vadāmi,",
"ja500:10.6": "Paññova seyyo na yasassi bālo”.",
"ja500:11.1": "“Dumaṃ yathā sāduphalaṃ araññe,",
"ja500:11.2": "Samantato samabhisaranti pakkhī;",
"ja500:11.3": "Evampi aḍḍhaṃ sadhanaṃ sabhogaṃ,",
"ja500:11.4": "Bahujjano bhajati atthahetu;",
"ja500:11.5": "Etampi disvāna ahaṃ vadāmi,",
"ja500:11.6": "Pañño nihīno sirīmāva seyyo”.",
"ja500:12.1": "“Na sādhu balavā bālo,",
"ja500:12.2": "Sāhasā vindate dhanaṃ;",
"ja500:12.3": "Kandantametaṃ dummedhaṃ,",
"ja500:12.4": "Kaḍḍhanti nirayaṃ bhusaṃ;",
"ja500:12.5": "Etampi disvāna ahaṃ vadāmi,",
"ja500:12.6": "Paññova seyyo na yasassi bālo”.",
"ja500:13.1": "“Yā kāci najjo gaṅgamabhissavanti,",
"ja500:13.2": "Sabbāva tā nāmagottaṃ jahanti;",
"ja500:13.3": "Gaṅgā samuddaṃ paṭipajjamānā,",
"ja500:13.4": "Na khāyate iddhiṃ paññopi loke;",
"ja500:13.5": "Etampi disvāna ahaṃ vadāmi,",
"ja500:13.6": "Pañño nihīno sirīmāva seyyo”.",
"ja500:14.1": "“Yametamakkhā udadhiṃ mahantaṃ,",
"ja500:14.2": "Savanti najjo sabbakālamasaṅkhyaṃ;",
"ja500:14.3": "So sāgaro niccamuḷāravego,",
"ja500:14.4": "Velaṃ na acceti mahāsamuddo.",
"ja500:15.1": "Evampi bālassa pajappitāni,",
"ja500:15.2": "Paññaṃ na acceti sirī kadāci;",
"ja500:15.3": "Etampi disvāna ahaṃ vadāmi,",
"ja500:15.4": "Paññova seyyo na yasassi bālo”.",
"ja500:16.1": "“Asaññato cepi paresamatthaṃ,",
"ja500:16.2": "Bhaṇāti sandhānagato yasassī;",
"ja500:16.3": "Tasseva taṃ rūhati ñātimajjhe,",
"ja500:16.4": "Sirī hi naṃ kārayate na paññā;",
"ja500:16.5": "Etampi disvāna ahaṃ vadāmi,",
"ja500:16.6": "Pañño nihīno sirīmāva seyyo”.",
"ja500:17.1": "“Parassa vā attano vāpi hetu,",
"ja500:17.2": "Bālo musā bhāsati appapañño;",
"ja500:17.3": "So nindito hoti sabhāya majjhe,",
"ja500:17.4": "Pacchāpi so duggatigāmī hoti;",
"ja500:17.5": "Etampi disvāna ahaṃ vadāmi,",
"ja500:17.6": "Paññova seyyo na yasassi bālo”.",
"ja500:18.1": "“Atthampi ce bhāsati bhūripañño,",
"ja500:18.2": "Anāḷhiyo appadhano daliddo;",
"ja500:18.3": "Na tassa taṃ rūhati ñātimajjhe,",
"ja500:18.4": "Sirī ca paññāṇavato na hoti;",
"ja500:18.5": "Etampi disvāna ahaṃ vadāmi,",
"ja500:18.6": "Pañño nihīno sirīmāva seyyo”.",
"ja500:19.1": "“Parassa vā attano vāpi hetu,",
"ja500:19.2": "Na bhāsati alikaṃ bhūripañño;",
"ja500:19.3": "So pūjito hoti sabhāya majjhe,",
"ja500:19.4": "Pacchāpi so suggatigāmī hoti;",
"ja500:19.5": "Etampi disvāna ahaṃ vadāmi,",
"ja500:19.6": "Paññova seyyo na yasassi bālo”.",
"ja500:20.1": "“Hatthī gavassā maṇikuṇḍalā ca,",
"ja500:20.2": "Thiyo ca iddhesu kulesu jātā;",
"ja500:20.3": "Sabbāva tā upabhogā bhavanti,",
"ja500:20.4": "Iddhassa posassa aniddhimanto;",
"ja500:20.5": "Etampi disvāna ahaṃ vadāmi,",
"ja500:20.6": "Pañño nihīno sirīmāva seyyo”.",
"ja500:21.1": "“Asaṃvihitakammantaṃ,",
"ja500:21.2": "Bālaṃ dummedhamantinaṃ;",
"ja500:21.3": "Sirī jahati dummedhaṃ,",
"ja500:21.4": "Jiṇṇaṃva urago tacaṃ;",
"ja500:21.5": "Etampi disvāna ahaṃ vadāmi,",
"ja500:21.6": "Paññova seyyo na yasassi bālo”.",
"ja500:22.1": "“Pañca paṇḍitā mayaṃ bhaddante,",
"ja500:22.2": "Sabbe pañjalikā upaṭṭhitā;",
"ja500:22.3": "Tvaṃ no abhibhuyya issarosi,",
"ja500:22.4": "Sakkova bhūtapati devarājā;",
"ja500:22.5": "Etampi disvāna ahaṃ vadāmi,",
"ja500:22.6": "Pañño nihīno sirīmāva seyyo”.",
"ja500:23.1": "“Dāsova paññassa yasassi bālo,",
"ja500:23.2": "Atthesu jātesu tathāvidhesu;",
"ja500:23.3": "Yaṃ paṇḍito nipuṇaṃ saṃvidheti,",
"ja500:23.4": "Sammohamāpajjati tattha bālo;",
"ja500:23.5": "Etampi disvāna ahaṃ vadāmi,",
"ja500:23.6": "Paññova seyyo na yasassi bālo.",
"ja500:24.1": "Addhā hi paññāva sataṃ pasatthā,",
"ja500:24.2": "Kantā sirī bhogaratā manussā;",
"ja500:24.3": "Ñāṇañca buddhānamatulyarūpaṃ,",
"ja500:24.4": "Paññaṃ na acceti sirī kadāci”.",
"ja500:25.1": "“Yaṃ taṃ apucchimha akittayī no,",
"ja500:25.2": "Mahosadha kevaladhammadassī;",
"ja500:25.3": "Gavaṃ sahassaṃ usabhañca nāgaṃ,",
"ja500:25.4": "Ājaññayutte ca rathe dasa ime;",
"ja500:25.5": "Pañhassa veyyākaraṇena tuṭṭho,",
"ja500:25.6": "Dadāmi te gāmavarāni soḷasā”ti.",
"ja500:26.1": "Sirīmantajātakaṃ catutthaṃ."
}