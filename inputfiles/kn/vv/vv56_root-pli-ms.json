{
"vv56:1.1": "Vimānavatthu",
"vv56:2.1": "Purisavimāna",
"vv56:3.1": "Mahārathavagga",
"vv56:4.1": "6. Paṭhamakaraṇīyavimānavatthu",
"vv56:5.1": "“Uccamidaṃ maṇithūṇaṃ vimānaṃ,",
"vv56:5.2": "Samantato dvādasa yojanāni;",
"vv56:5.3": "Kūṭāgārā sattasatā uḷārā,",
"vv56:5.4": "Veḷuriyathambhā rucakatthatā subhā.",
"vv56:6.1": "Tatthacchasi pivasi khādasi ca,",
"vv56:6.2": "Dibbā ca vīṇā pavadanti vagguṃ;",
"vv56:6.3": "Dibbā rasā kāmaguṇettha pañca,",
"vv56:6.4": "Nāriyo ca naccanti suvaṇṇachannā.",
"vv56:7.1": "Kena tetādiso vaṇṇo, … pe …",
"vv56:7.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
"vv56:8.1": "So devaputto attamano, … pe …",
"vv56:8.4": "yassa kammassidaṃ phalaṃ.",
"vv56:9.1": "“Karaṇīyāni puññāni,",
"vv56:9.2": "paṇḍitena vijānatā;",
"vv56:9.3": "Sammaggatesu buddhesu,",
"vv56:9.4": "yattha dinnaṃ mahapphalaṃ.",
"vv56:10.1": "Atthāya vata me buddho,",
"vv56:10.2": "araññā gāmamāgato;",
"vv56:10.3": "Tattha cittaṃ pasādetvā,",
"vv56:10.4": "tāvatiṃsūpago ahaṃ.",
"vv56:11.1": "Tena metādiso vaṇṇo, … pe …",
"vv56:11.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
"vv56:12.1": "Paṭhamakaraṇīyavimānaṃ chaṭṭhaṃ."
}