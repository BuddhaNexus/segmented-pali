{
  "vv23:0.1": "Vimānavatthu ",
  "vv23:0.2": "Itthivimāna ",
  "vv23:0.3": "Cittalatāvagga ",
  "vv23:0.4": "6. Soṇadinnāvimānavatthu ",
  "vv23:1.1": "“Abhikkantena vaṇṇena, ",
  "vv23:1.2": "yā tvaṁ tiṭṭhasi devate; ",
  "vv23:1.3": "Obhāsentī disā sabbā, ",
  "vv23:1.4": "osadhī viya tārakā. ",
  "vv23:2.1": "Kena tetādiso vaṇṇo, ",
  "vv23:2.2": "kena te idha mijjhati; ",
  "vv23:2.3": "Uppajjanti ca te bhogā, ",
  "vv23:2.4": "ye keci manaso piyā. ",
  "vv23:3.1": "Pucchāmi taṁ devi mahānubhāve, ",
  "vv23:3.2": "Manussabhūtā kimakāsi puññaṁ; ",
  "vv23:3.3": "Kenāsi evaṁ jalitānubhāvā, ",
  "vv23:3.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti. ",
  "vv23:4.1": "Sā devatā attamanā, ",
  "vv23:4.2": "moggallānena pucchitā; ",
  "vv23:4.3": "Pañhaṁ puṭṭhā viyākāsi, ",
  "vv23:4.4": "yassa kammassidaṁ phalaṁ. ",
  "vv23:5.1": "“Soṇadinnāti maṁ aññaṁsu, ",
  "vv23:5.2": "nāḷandāyaṁ upāsikā; ",
  "vv23:5.3": "Saddhā sīlena sampannā, ",
  "vv23:5.4": "saṁvibhāgaratā sadā. ",
  "vv23:6.1": "Acchādanañca bhattañca, ",
  "vv23:6.2": "senāsanaṁ padīpiyaṁ; ",
  "vv23:6.3": "Adāsiṁ ujubhūtesu, ",
  "vv23:6.4": "vippasannena cetasā. ",
  "vv23:7.1": "Cātuddasiṁ pañcadasiṁ, ",
  "vv23:7.2": "yā ca pakkhassa aṭṭhamī; ",
  "vv23:7.3": "Pāṭihāriyapakkhañca, ",
  "vv23:7.4": "aṭṭhaṅgasusamāgataṁ. ",
  "vv23:8.1": "Uposathaṁ upavasissaṁ, ",
  "vv23:8.2": "sadā sīlesu saṁvutā; ",
  "vv23:8.3": "Saññamā saṁvibhāgā ca, ",
  "vv23:8.4": "vimānaṁ āvasāmahaṁ. ",
  "vv23:9.1": "Pāṇātipātā viratā, ",
  "vv23:9.2": "musāvādā ca saññatā; ",
  "vv23:9.3": "Theyyā ca aticārā ca, ",
  "vv23:9.4": "majjapānā ca ārakā. ",
  "vv23:10.1": "Pañcasikkhāpade ratā, ",
  "vv23:10.2": "ariyasaccāna kovidā; ",
  "vv23:10.3": "Upāsikā cakkhumato, ",
  "vv23:10.4": "gotamassa yasassino. ",
  "vv23:11.1": "Tena metādiso vaṇṇo, ",
  "vv23:11.2": "…pe… ",
  "vv23:11.3": "Vaṇṇo ca me sabbadisā pabhāsatī”ti. ",
  "vv23:12.1": "Soṇadinnāvimānaṁ chaṭṭhaṁ. "
}