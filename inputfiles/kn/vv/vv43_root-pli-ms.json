{
  "vv43:0.1": "Vimānavatthu ",
  "vv43:0.2": "Itthivimāna ",
  "vv43:0.3": "Mañjiṭṭhakavagga ",
  "vv43:0.4": "5. Kañjikadāyikāvimānavatthu ",
  "vv43:1.1": "“Abhikkantena vaṇṇena, ",
  "vv43:1.2": "…pe… ",
  "vv43:1.3": "osadhī viya tārakā. ",
  "vv43:2.1": "Kena tetādiso vaṇṇo, ",
  "vv43:2.2": "…pe… ",
  "vv43:2.3": "Vaṇṇo ca te sabbadisā pabhāsatī”ti. ",
  "vv43:3.1": "Sā devatā attamanā, ",
  "vv43:3.2": "…pe… ",
  "vv43:3.3": "yassa kammassidaṁ phalaṁ. ",
  "vv43:4.1": "“Ahaṁ andhakavindamhi, ",
  "vv43:4.2": "buddhassādiccabandhuno; ",
  "vv43:4.3": "Adāsiṁ kolasampākaṁ, ",
  "vv43:4.4": "kañjikaṁ teladhūpitaṁ. ",
  "vv43:5.1": "Pipphalyā lasuṇena ca, ",
  "vv43:5.2": "missaṁ lāmañjakena ca; ",
  "vv43:5.3": "Adāsiṁ ujubhūtasmiṁ, ",
  "vv43:5.4": "vippasannena cetasā. ",
  "vv43:6.1": "Yā mahesittaṁ kāreyya, ",
  "vv43:6.2": "cakkavattissa rājino; ",
  "vv43:6.3": "Nārī sabbaṅgakalyāṇī, ",
  "vv43:6.4": "bhattu cānomadassikā; ",
  "vv43:6.5": "Ekassa kañjikadānassa, ",
  "vv43:6.6": "kalaṁ nāgghati soḷasiṁ. ",
  "vv43:7.1": "Sataṁ nikkhā sataṁ assā, ",
  "vv43:7.2": "sataṁ assatarīrathā; ",
  "vv43:7.3": "Sataṁ kaññāsahassāni, ",
  "vv43:7.4": "āmuttamaṇikuṇḍalā; ",
  "vv43:7.5": "Ekassa kañjikadānassa, ",
  "vv43:7.6": "kalaṁ nāgghanti soḷasiṁ. ",
  "vv43:8.1": "Sataṁ hemavatā nāgā, ",
  "vv43:8.2": "īsādantā urūḷhavā; ",
  "vv43:8.3": "Suvaṇṇakacchā mātaṅgā, ",
  "vv43:8.4": "hemakappanavāsasā; ",
  "vv43:8.5": "Ekassa kañjikadānassa, ",
  "vv43:8.6": "kalaṁ nāgghanti soḷasiṁ. ",
  "vv43:9.1": "Catunnamapi dīpānaṁ, ",
  "vv43:9.2": "issaraṁ yodha kāraye; ",
  "vv43:9.3": "Ekassa kañjikadānassa, ",
  "vv43:9.4": "kalaṁ nāgghati soḷasin”ti. ",
  "vv43:10.1": "Kañjikadāyikāvimānaṁ pañcamaṁ. "
}