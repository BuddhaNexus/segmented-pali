{
  "vv36:0.1": "Vimānavatthu ",
  "vv36:0.2": "Itthivimāna ",
  "vv36:0.3": "Pāricchattakavagga ",
  "vv36:0.4": "8. Mallikāvimānavatthu ",
  "vv36:1.1": "“Pītavatthe pītadhaje, ",
  "vv36:1.2": "pītālaṅkārabhūsite; ",
  "vv36:1.3": "Pītantarāhi vaggūhi, ",
  "vv36:1.4": "apiḷandhāva sobhasi. ",
  "vv36:2.1": "Kā kambukāyūradhare, ",
  "vv36:2.2": "kañcanāveḷabhūsite; ",
  "vv36:2.3": "Hemajālakasañchanne, ",
  "vv36:2.4": "nānāratanamālinī. ",
  "vv36:3.1": "Sovaṇṇamayā lohitaṅgamayā ca, ",
  "vv36:3.2": "Muttāmayā veḷuriyamayā ca; ",
  "vv36:3.3": "Masāragallā sahalohitaṅgā, ",
  "vv36:3.4": "Pārevatakkhīhi maṇīhi cittatā. ",
  "vv36:4.1": "Koci koci ettha mayūrasussaro, ",
  "vv36:4.2": "Haṁsassa rañño karavīkasussaro; ",
  "vv36:4.3": "Tesaṁ saro suyyati vaggurūpo, ",
  "vv36:4.4": "Pañcaṅgikaṁ tūriyamivappavāditaṁ. ",
  "vv36:5.1": "Ratho ca te subho vaggu, ",
  "vv36:5.2": "nānāratanacittito; ",
  "vv36:5.3": "Nānāvaṇṇāhi dhātūhi, ",
  "vv36:5.4": "suvibhattova sobhati. ",
  "vv36:6.1": "Tasmiṁ rathe kañcanabimbavaṇṇe, ",
  "vv36:6.2": "Yā tvaṁ ṭhitā bhāsasimaṁ padesaṁ; ",
  "vv36:6.3": "Devate pucchitācikkha, ",
  "vv36:6.4": "Kissa kammassidaṁ phalan”ti. ",
  "vv36:7.1": "“Sovaṇṇajālaṁ maṇisoṇṇacittitaṁ, ",
  "vv36:7.2": "Muttācitaṁ hemajālena channaṁ; ",
  "vv36:7.3": "Parinibbute gotame appameyye, ",
  "vv36:7.4": "Pasannacittā ahamābhiropayiṁ. ",
  "vv36:8.1": "Tāhaṁ kammaṁ karitvāna, ",
  "vv36:8.2": "kusalaṁ buddhavaṇṇitaṁ; ",
  "vv36:8.3": "Apetasokā sukhitā, ",
  "vv36:8.4": "sampamodāmanāmayā”ti. ",
  "vv36:9.1": "Mallikāvimānaṁ aṭṭhamaṁ. "
}