{
"vv79:1.1": "Vimānavatthu",
"vv79:2.1": "Purisavimāna",
"vv79:3.1": "Sunikkhittavagga",
"vv79:4.1": "5. Ambavimānavatthu",
"vv79:5.1": "“Uccamidaṃ maṇithūṇaṃ vimānaṃ,",
"vv79:5.2": "Samantato dvādasa yojanāni;",
"vv79:5.3": "Kūṭāgārā sattasatā uḷārā,",
"vv79:5.4": "Veḷuriyathambhā rucakatthatā subhā.",
"vv79:6.1": "Tatthacchasi pivasi khādasi ca,",
"vv79:6.2": "Dibbā ca vīṇā pavadanti vagguṃ;",
"vv79:6.3": "Dibbā rasā kāmaguṇettha pañca,",
"vv79:6.4": "Nāriyo ca naccanti suvaṇṇachannā.",
"vv79:7.1": "Kena tetādiso vaṇṇo, … pe …",
"vv79:7.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
"vv79:8.1": "So devaputto attamano, … pe …",
"vv79:8.4": "yassa kammassidaṃ phalaṃ.",
"vv79:9.1": "“Gimhānaṃ pacchime māse,",
"vv79:9.2": "patapante divaṅkare;",
"vv79:9.3": "Paresaṃ bhatako poso,",
"vv79:9.4": "ambārāmamasiñcati.",
"vv79:10.1": "Atha tenāgamā bhikkhu,",
"vv79:10.2": "sāriputtoti vissuto;",
"vv79:10.3": "Kilantarūpo kāyena,",
"vv79:10.4": "akilantova cetasā.",
"vv79:11.1": "Tañca disvāna āyantaṃ,",
"vv79:11.2": "Avocaṃ ambasiñcako;",
"vv79:11.3": "Sādhu taṃ bhante nhāpeyyaṃ,",
"vv79:11.4": "Yaṃ mamassa sukhāvahaṃ.",
"vv79:12.1": "Tassa me anukampāya,",
"vv79:12.2": "nikkhipi pattacīvaraṃ;",
"vv79:12.3": "Nisīdi rukkhamūlasmiṃ,",
"vv79:12.4": "chāyāya ekacīvaro.",
"vv79:13.1": "Tañca acchena vārinā,",
"vv79:13.2": "pasannamānaso naro;",
"vv79:13.3": "Nhāpayī rukkhamūlasmiṃ,",
"vv79:13.4": "chāyāya ekacīvaraṃ.",
"vv79:14.1": "Ambo ca sitto samaṇo ca nhāpito,",
"vv79:14.2": "Mayā ca puññaṃ pasutaṃ anappakaṃ;",
"vv79:14.3": "Iti so pītiyā kāyaṃ,",
"vv79:14.4": "Sabbaṃ pharati attano.",
"vv79:15.1": "Tadeva ettakaṃ kammaṃ,",
"vv79:15.2": "akāsiṃ tāya jātiyā;",
"vv79:15.3": "Pahāya mānusaṃ dehaṃ,",
"vv79:15.4": "upapannomhi nandanaṃ.",
"vv79:16.1": "Nandane ca vane ramme,",
"vv79:16.2": "nānādijagaṇāyute;",
"vv79:16.3": "Ramāmi naccagītehi,",
"vv79:16.4": "accharāhi purakkhato”ti.",
"vv79:17.1": "Ambavimānaṃ pañcamaṃ."
}