{
"vv10:1.1": "Vimānavatthu",
"vv10:2.1": "Itthivimāna",
"vv10:3.1": "Pīṭhavagga",
"vv10:4.1": "10. Tiladakkhiṇavimānavatthu",
"vv10:5.1": "“Abhikkantena vaṇṇena,",
"vv10:5.2": "yā tvaṃ tiṭṭhasi devate;",
"vv10:5.3": "Obhāsentī disā sabbā,",
"vv10:5.4": "osadhī viya tārakā.",
"vv10:6.1": "Kena tetādiso vaṇṇo,",
"vv10:6.2": "kena te idha mijjhati;",
"vv10:6.3": "Uppajjanti ca te bhogā,",
"vv10:6.4": "ye keci manaso piyā.",
"vv10:7.1": "Pucchāmi taṃ devi mahānubhāve,",
"vv10:7.2": "Manussabhūtā kimakāsi puññaṃ;",
"vv10:7.3": "Kenāsi evaṃ jalitānubhāvā,",
"vv10:7.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
"vv10:8.1": "Sā devatā attamanā,",
"vv10:8.2": "moggallānena pucchitā;",
"vv10:8.3": "Pañhaṃ puṭṭhā viyākāsi,",
"vv10:8.4": "yassa kammassidaṃ phalaṃ.",
"vv10:9.1": "“Ahaṃ manussesu manussabhūtā,",
"vv10:9.2": "Purimāya jātiyā manussaloke.",
"vv10:10.1": "Addasaṃ virajaṃ buddhaṃ,",
"vv10:10.2": "vippasannamanāvilaṃ;",
"vv10:10.3": "Āsajja dānaṃ adāsiṃ,",
"vv10:10.4": "akāmā tiladakkhiṇaṃ;",
"vv10:10.5": "Dakkhiṇeyyassa buddhassa,",
"vv10:10.6": "pasannā sehi pāṇibhi.",
"vv10:11.1": "Tena metādiso vaṇṇo,",
"vv10:11.2": "tena me idha mijjhati;",
"vv10:11.3": "Uppajjanti ca me bhogā,",
"vv10:11.4": "ye keci manaso piyā.",
"vv10:12.1": "Akkhāmi te bhikkhu mahānubhāva,",
"vv10:12.2": "Manussabhūtā yamakāsi puññaṃ;",
"vv10:12.3": "Tenamhi evaṃ jalitānubhāvā,",
"vv10:12.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
"vv10:13.1": "Tiladakkhiṇavimānaṃ dasamaṃ."
}