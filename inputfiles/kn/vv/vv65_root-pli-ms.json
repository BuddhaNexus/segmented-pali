{
"vv65:1.1": "Vimānavatthu",
"vv65:2.1": "Purisavimāna",
"vv65:3.1": "Pāyāsivagga",
"vv65:4.1": "1. Paṭhamaagāriyavimānavatthu",
"vv65:5.1": "“Yathā vanaṃ cittalataṃ pabhāsati,",
"vv65:5.2": "Uyyānaseṭṭhaṃ tidasānamuttamaṃ;",
"vv65:5.3": "Tathūpamaṃ tuyhamidaṃ vimānaṃ,",
"vv65:5.4": "Obhāsayaṃ tiṭṭhati antalikkhe.",
"vv65:6.1": "Deviddhipattosi mahānubhāvo,",
"vv65:6.2": "Manussabhūto kimakāsi puññaṃ;",
"vv65:6.3": "Kenāsi evaṃ jalitānubhāvo,",
"vv65:6.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
"vv65:7.1": "So devaputto attamano,",
"vv65:7.2": "moggallānena pucchito;",
"vv65:7.3": "Pañhaṃ puṭṭho viyākāsi,",
"vv65:7.4": "yassa kammassidaṃ phalaṃ.",
"vv65:8.1": "“Ahañca bhariyā ca manussaloke,",
"vv65:8.2": "Opānabhūtā gharamāvasimha;",
"vv65:8.3": "Annañca pānañca pasannacittā,",
"vv65:8.4": "Sakkacca dānaṃ vipulaṃ adamha.",
"vv65:9.1": "Tena metādiso vaṇṇo, … pe …",
"vv65:9.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
"vv65:10.1": "Paṭhamaagāriyavimānaṃ paṭhamaṃ."
}