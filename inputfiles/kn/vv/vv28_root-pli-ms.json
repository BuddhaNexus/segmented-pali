{
"vv28:1.1": "Vimānavatthu",
"vv28:2.1": "Itthivimāna",
"vv28:3.1": "Cittalatāvagga",
"vv28:4.1": "11. Dutiyabhikkhādāyikāvimānavatthu",
"vv28:5.1": "“Abhikkantena vaṇṇena,",
"vv28:5.2": "yā tvaṃ tiṭṭhasi devate;",
"vv28:5.3": "Obhāsentī disā sabbā,",
"vv28:5.4": "osadhī viya tārakā.",
"vv28:6.1": "Kena tetādiso vaṇṇo, … pe …",
"vv28:6.4": "vaṇṇo ca te sabbadisā pabhāsatī”ti.",
"vv28:7.1": "Sā devatā attamanā, … pe …",
"vv28:7.4": "yassa kammassidaṃ phalaṃ.",
"vv28:8.1": "“Ahaṃ manussesu manussabhūtā,",
"vv28:8.2": "Purimāya jātiyā manussaloke.",
"vv28:9.1": "Addasaṃ virajaṃ bhikkhuṃ,",
"vv28:9.2": "vippasannamanāvilaṃ;",
"vv28:9.3": "Tassa adāsahaṃ bhikkhaṃ,",
"vv28:9.4": "pasannā sehi pāṇibhi.",
"vv28:10.1": "Tena metādiso vaṇṇo, … pe …",
"vv28:10.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
"vv28:11.1": "Dutiyabhikkhādāyikāvimānaṃ ekādasamaṃ.",
"vv28:12.1": "Tassuddānaṃ",
"vv28:13.1": "Dāsī ceva lakhumā ca,",
"vv28:13.2": "atha ācāmadāyikā;",
"vv28:13.3": "Caṇḍālī bhadditthī ceva,",
"vv28:13.4": "soṇadinnā uposathā;",
"vv28:13.5": "Niddā ceva suniddā ca,",
"vv28:13.6": "dve ca bhikkhāya dāyikā;",
"vv28:13.7": "Vaggo tena pavuccatīti.",
"vv28:14.1": "Itthivimāne dutiyo vaggo.",
"vv28:15.1": "Bhāṇavāraṃ paṭhamaṃ."
}