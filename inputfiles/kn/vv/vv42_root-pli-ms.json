{
"vv42:1.1": "Vimānavatthu",
"vv42:2.1": "Itthivimāna",
"vv42:3.1": "Mañjiṭṭhakavagga",
"vv42:4.1": "4. Alomavimānavatthu",
"vv42:5.1": "“Abhikkantena vaṇṇena,",
"vv42:5.2": "yā tvaṃ tiṭṭhasi devate;",
"vv42:5.3": "Obhāsentī disā sabbā,",
"vv42:5.4": "osadhī viya tārakā.",
"vv42:6.1": "Kena tetādiso vaṇṇo, … pe …",
"vv42:6.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
"vv42:7.1": "Sā devatā attamanā, … pe …",
"vv42:7.4": "yassa kammassidaṃ phalaṃ.",
"vv42:8.1": "“Ahañca bārāṇasiyaṃ,",
"vv42:8.2": "buddhassādiccabandhuno;",
"vv42:8.3": "Adāsiṃ sukkhakummāsaṃ,",
"vv42:8.4": "pasannā sehi pāṇibhi.",
"vv42:9.1": "Sukkhāya aloṇikāya ca,",
"vv42:9.2": "Passa phalaṃ kummāsapiṇḍiyā;",
"vv42:9.3": "Alomaṃ sukhitaṃ disvā,",
"vv42:9.4": "Ko puññaṃ na karissati.",
"vv42:10.1": "Tena metādiso vaṇṇo, … pe …",
"vv42:10.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
"vv42:11.1": "Alomavimānaṃ catutthaṃ."
}