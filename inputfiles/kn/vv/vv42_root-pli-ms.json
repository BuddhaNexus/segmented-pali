{
  "vv42:0.1": "Vimānavatthu ",
  "vv42:0.2": "Itthivimāna ",
  "vv42:0.3": "Mañjiṭṭhakavagga ",
  "vv42:0.4": "4. Alomavimānavatthu ",
  "vv42:1.1": "“Abhikkantena vaṇṇena, ",
  "vv42:1.2": "yā tvaṁ tiṭṭhasi devate; ",
  "vv42:1.3": "Obhāsentī disā sabbā, ",
  "vv42:1.4": "osadhī viya tārakā. ",
  "vv42:2.1": "Kena tetādiso vaṇṇo, ",
  "vv42:2.2": "…pe… ",
  "vv42:2.3": "Vaṇṇo ca te sabbadisā pabhāsatī”ti. ",
  "vv42:3.1": "Sā devatā attamanā, ",
  "vv42:3.2": "…pe… ",
  "vv42:3.3": "yassa kammassidaṁ phalaṁ. ",
  "vv42:4.1": "“Ahañca bārāṇasiyaṁ, ",
  "vv42:4.2": "buddhassādiccabandhuno; ",
  "vv42:4.3": "Adāsiṁ sukkhakummāsaṁ, ",
  "vv42:4.4": "pasannā sehi pāṇibhi. ",
  "vv42:5.1": "Sukkhāya aloṇikāya ca, ",
  "vv42:5.2": "Passa phalaṁ kummāsapiṇḍiyā; ",
  "vv42:5.3": "Alomaṁ sukhitaṁ disvā, ",
  "vv42:5.4": "Ko puññaṁ na karissati. ",
  "vv42:6.1": "Tena metādiso vaṇṇo, ",
  "vv42:6.2": "…pe… ",
  "vv42:6.3": "Vaṇṇo ca me sabbadisā pabhāsatī”ti. ",
  "vv42:7.1": "Alomavimānaṁ catutthaṁ. "
}