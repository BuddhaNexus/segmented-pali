{
"vv73:1.1": "Vimānavatthu",
"vv73:2.1": "Purisavimāna",
"vv73:3.1": "Pāyāsivagga",
"vv73:4.1": "9. Dutiyakuṇḍalīvimānavatthu",
"vv73:5.1": "“Alaṅkato malyadharo suvattho,",
"vv73:5.2": "Sukuṇḍalī kappitakesamassu;",
"vv73:5.3": "Āmuttahatthābharaṇo yasassī,",
"vv73:5.4": "Dibbe vimānamhi yathāpi candimā.",
"vv73:6.1": "Dibbā ca vīṇā pavadanti vagguṃ,",
"vv73:6.2": "Aṭṭhaṭṭhakā sikkhitā sādhurūpā;",
"vv73:6.3": "Dibbā ca kaññā tidasacarā uḷārā,",
"vv73:6.4": "Naccanti gāyanti pamodayanti.",
"vv73:7.1": "Deviddhipattosi mahānubhāvo,",
"vv73:7.2": "Manussabhūto kimakāsi puññaṃ;",
"vv73:7.3": "Kenāsi evaṃ jalitānubhāvo,",
"vv73:7.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
"vv73:8.1": "So devaputto attamano, … pe …",
"vv73:8.4": "yassa kammassidaṃ phalaṃ.",
"vv73:9.1": "“Ahaṃ manussesu manussabhūto,",
"vv73:9.2": "Disvāna samaṇe sādhurūpe;",
"vv73:9.3": "Sampannavijjācaraṇe yasassī,",
"vv73:9.4": "Bahussute sīlavante pasanne;",
"vv73:9.5": "Annañca pānañca pasannacitto,",
"vv73:9.6": "Sakkacca dānaṃ vipulaṃ adāsiṃ.",
"vv73:10.1": "Tena metādiso vaṇṇo, … pe …",
"vv73:10.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
"vv73:11.1": "Dutiyakuṇḍalīvimānaṃ navamaṃ."
}