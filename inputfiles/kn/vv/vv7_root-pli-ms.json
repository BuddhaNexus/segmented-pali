{
  "vv7:0.1": "Vimānavatthu ",
  "vv7:0.2": "Itthivimāna ",
  "vv7:0.3": "Pīṭhavagga ",
  "vv7:0.4": "7. Dutiyanāvāvimānavatthu ",
  "vv7:1.1": "“Suvaṇṇacchadanaṁ nāvaṁ, ",
  "vv7:1.2": "Nāri āruyha tiṭṭhasi; ",
  "vv7:1.3": "Ogāhasi pokkharaṇiṁ, ",
  "vv7:1.4": "Padmaṁ chindasi pāṇinā. ",
  "vv7:2.1": "Kena tetādiso vaṇṇo, ",
  "vv7:2.2": "kena te idha mijjhati; ",
  "vv7:2.3": "Uppajjanti ca te bhogā, ",
  "vv7:2.4": "ye keci manaso piyā. ",
  "vv7:3.1": "Pucchāmi taṁ devi mahānubhāve, ",
  "vv7:3.2": "Manussabhūtā kimakāsi puññaṁ; ",
  "vv7:3.3": "Kenāsi evaṁ jalitānubhāvā, ",
  "vv7:3.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti. ",
  "vv7:4.1": "Sā devatā attamanā, ",
  "vv7:4.2": "moggallānena pucchitā; ",
  "vv7:4.3": "Pañhaṁ puṭṭhā viyākāsi, ",
  "vv7:4.4": "yassa kammassidaṁ phalaṁ. ",
  "vv7:5.1": "“Ahaṁ manussesu manussabhūtā, ",
  "vv7:5.2": "Purimāya jātiyā manussaloke; ",
  "vv7:5.3": "Disvāna bhikkhuṁ tasitaṁ kilantaṁ, ",
  "vv7:5.4": "Uṭṭhāya pātuṁ udakaṁ adāsiṁ. ",
  "vv7:6.1": "Yo ve kilantassa pipāsitassa, ",
  "vv7:6.2": "Uṭṭhāya pātuṁ udakaṁ dadāti; ",
  "vv7:6.3": "Sītodakā tassa bhavanti najjo, ",
  "vv7:6.4": "Pahūtamalyā bahupuṇḍarīkā. ",
  "vv7:7.1": "Taṁ āpagā anupariyanti sabbadā, ",
  "vv7:7.2": "Sītodakā vālukasanthatā nadī; ",
  "vv7:7.3": "Ambā ca sālā tilakā ca jambuyo, ",
  "vv7:7.4": "Uddālakā pāṭaliyo ca phullā. ",
  "vv7:8.1": "Taṁ bhūmibhāgehi upetarūpaṁ, ",
  "vv7:8.2": "Vimānaseṭṭhaṁ bhusa sobhamānaṁ; ",
  "vv7:8.3": "Tassīdha kammassa ayaṁ vipāko, ",
  "vv7:8.4": "Etādisaṁ puññakatā labhanti. ",
  "vv7:9.1": "Tena metādiso vaṇṇo, ",
  "vv7:9.2": "tena me idha mijjhati; ",
  "vv7:9.3": "Uppajjanti ca me bhogā, ",
  "vv7:9.4": "ye keci manaso piyā. ",
  "vv7:10.1": "Akkhāmi te bhikkhu mahānubhāva, ",
  "vv7:10.2": "Manussabhūtā yamakāsi puññaṁ; ",
  "vv7:10.3": "Tenamhi evaṁ jalitānubhāvā, ",
  "vv7:10.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti. ",
  "vv7:11.1": "Dutiyanāvāvimānaṁ sattamaṁ. "
}