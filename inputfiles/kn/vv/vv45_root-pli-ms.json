{
"vv45:1.1": "Vimānavatthu",
"vv45:2.1": "Itthivimāna",
"vv45:3.1": "Mañjiṭṭhakavagga",
"vv45:4.1": "7. Caturitthivimānavatthu",
"vv45:5.1": "“Abhikkantena vaṇṇena, … pe …",
"vv45:5.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
"vv45:6.1": "Sā devatā attamanā, … pe …",
"vv45:6.4": "yassa kammassidaṃ phalaṃ.",
"vv45:7.1": "“Indīvarānaṃ hatthakaṃ ahamadāsiṃ,",
"vv45:7.2": "Bhikkhuno piṇḍāya carantassa;",
"vv45:7.3": "Esikānaṃ uṇṇatasmiṃ,",
"vv45:7.4": "Nagaravare paṇṇakate ramme.",
"vv45:8.1": "Tena metādiso vaṇṇo, … pe …",
"vv45:8.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
"vv45:9.1": "“Abhikkantena vaṇṇena, … pe …",
"vv45:9.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
"vv45:10.1": "Sā devatā attamanā, … pe …",
"vv45:10.4": "yassa kammassidaṃ phalaṃ.",
"vv45:11.1": "“Nīluppalahatthakaṃ ahamadāsiṃ,",
"vv45:11.2": "Bhikkhuno piṇḍāya carantassa;",
"vv45:11.3": "Esikānaṃ uṇṇatasmiṃ,",
"vv45:11.4": "Nagaravare paṇṇakate ramme.",
"vv45:12.1": "Tena metādiso vaṇṇo, … pe …",
"vv45:12.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
"vv45:13.1": "“Abhikkantena vaṇṇena, … pe …",
"vv45:13.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
"vv45:14.1": "Sā devatā attamanā, … pe …",
"vv45:14.4": "yassa kammassidaṃ phalaṃ.",
"vv45:15.1": "“Odātamūlakaṃ haritapattaṃ,",
"vv45:15.2": "Udakasmiṃ sare jātaṃ ahamadāsiṃ;",
"vv45:15.3": "Bhikkhuno piṇḍāya carantassa,",
"vv45:15.4": "Esikānaṃ uṇṇatasmiṃ;",
"vv45:15.5": "Nagaravare paṇṇakate ramme.",
"vv45:16.1": "Tena metādiso vaṇṇo, … pe …",
"vv45:16.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
"vv45:17.1": "“Abhikkantena vaṇṇena, … pe …",
"vv45:17.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
"vv45:18.1": "Sā devatā attamanā, … pe …",
"vv45:18.4": "yassa kammassidaṃ phalaṃ.",
"vv45:19.1": "“Ahaṃ sumanā sumanassa sumanamakuḷāni,",
"vv45:19.2": "Dantavaṇṇāni ahamadāsiṃ;",
"vv45:19.3": "Bhikkhuno piṇḍāya carantassa,",
"vv45:19.4": "Esikānaṃ uṇṇatasmiṃ;",
"vv45:19.5": "Nagaravare paṇṇakate ramme.",
"vv45:20.1": "Tena metādiso vaṇṇo, … pe …",
"vv45:20.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
"vv45:21.1": "Caturitthivimānaṃ sattamaṃ."
}