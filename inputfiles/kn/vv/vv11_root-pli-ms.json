{
"vv11:1.1": "Vimānavatthu",
"vv11:2.1": "Itthivimāna",
"vv11:3.1": "Pīṭhavagga",
"vv11:4.1": "11. Paṭhamapatibbatāvimānavatthu",
"vv11:5.1": "“Koñcā mayūrā diviyā ca haṃsā,",
"vv11:5.2": "Vaggussarā kokilā sampatanti;",
"vv11:5.3": "Pupphābhikiṇṇaṃ rammamidaṃ vimānaṃ,",
"vv11:5.4": "Anekacittaṃ naranārisevitaṃ.",
"vv11:6.1": "Tatthacchasi devi mahānubhāve,",
"vv11:6.2": "Iddhī vikubbanti anekarūpā;",
"vv11:6.3": "Imā ca te accharāyo samantato,",
"vv11:6.4": "Naccanti gāyanti pamodayanti ca.",
"vv11:7.1": "Deviddhipattāsi mahānubhāve,",
"vv11:7.2": "Manussabhūtā kimakāsi puññaṃ;",
"vv11:7.3": "Kenāsi evaṃ jalitānubhāvā,",
"vv11:7.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
"vv11:8.1": "Sā devatā attamanā,",
"vv11:8.2": "moggallānena pucchitā;",
"vv11:8.3": "Pañhaṃ puṭṭhā viyākāsi,",
"vv11:8.4": "yassa kammassidaṃ phalaṃ.",
"vv11:9.1": "“Ahaṃ manussesu manussabhūtā,",
"vv11:9.2": "Patibbatānaññamanā ahosiṃ;",
"vv11:9.3": "Mātāva puttaṃ anurakkhamānā,",
"vv11:9.4": "Kuddhāpihaṃ nappharusaṃ avocaṃ.",
"vv11:10.1": "Sacce ṭhitā mosavajjaṃ pahāya,",
"vv11:10.2": "Dāne ratā saṅgahitattabhāvā;",
"vv11:10.3": "Annañca pānañca pasannacittā,",
"vv11:10.4": "Sakkacca dānaṃ vipulaṃ adāsiṃ.",
"vv11:11.1": "Tena metādiso vaṇṇo,",
"vv11:11.2": "tena me idha mijjhati;",
"vv11:11.3": "Uppajjanti ca me bhogā,",
"vv11:11.4": "ye keci manaso piyā.",
"vv11:12.1": "Akkhāmi te bhikkhu mahānubhāva,",
"vv11:12.2": "Manussabhūtā yamakāsi puññaṃ;",
"vv11:12.3": "Tenamhi evaṃ jalitānubhāvā,",
"vv11:12.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
"vv11:13.1": "Paṭhamapatibbatāvimānaṃ ekādasamaṃ."
}