{
"vv60:1.1": "Vimānavatthu",
"vv60:2.1": "Purisavimāna",
"vv60:3.1": "Mahārathavagga",
"vv60:4.1": "10. Paṭhamanāgavimānavatthu",
"vv60:5.1": "“Susukkakhandhaṃ abhiruyha nāgaṃ,",
"vv60:5.2": "Akācinaṃ dantiṃ baliṃ mahājavaṃ;",
"vv60:5.3": "Abhiruyha gajavaraṃ sukappitaṃ,",
"vv60:5.4": "Idhāgamā vehāyasaṃ antalikkhe.",
"vv60:6.1": "Nāgassa dantesu duvesu nimmitā,",
"vv60:6.2": "Acchodakā paduminiyo suphullā;",
"vv60:6.3": "Padumesu ca tūriyagaṇā pavajjare,",
"vv60:6.4": "Imā ca naccanti manoharāyo.",
"vv60:7.1": "Deviddhipattosi mahānubhāvo,",
"vv60:7.2": "Manussabhūto kimakāsi puññaṃ;",
"vv60:7.3": "Kenāsi evaṃ jalitānubhāvo,",
"vv60:7.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
"vv60:8.1": "So devaputto attamano,",
"vv60:8.2": "moggallānena pucchito;",
"vv60:8.3": "Pañhaṃ puṭṭho viyākāsi,",
"vv60:8.4": "yassa kammassidaṃ phalaṃ.",
"vv60:9.1": "“Aṭṭheva muttapupphāni,",
"vv60:9.2": "kassapassa mahesino;",
"vv60:9.3": "Thūpasmiṃ abhiropesiṃ,",
"vv60:9.4": "pasanno sehi pāṇibhi.",
"vv60:10.1": "Tena metādiso vaṇṇo, … pe …",
"vv60:10.4": "vaṇṇo ca me sabbadisā pabhāsatī”ti.",
"vv60:11.1": "Paṭhamanāgavimānaṃ dasamaṃ."
}