{
  "vv18:0.1": "Vimānavatthu ",
  "vv18:0.2": "Itthivimāna ",
  "vv18:0.3": "Cittalatāvagga ",
  "vv18:0.4": "1. Dāsivimānavatthu ",
  "vv18:1.1": "“Api sakkova devindo, ",
  "vv18:1.2": "ramme cittalatāvane; ",
  "vv18:1.3": "Samantā anupariyāsi, ",
  "vv18:1.4": "nārīgaṇapurakkhatā; ",
  "vv18:1.5": "Obhāsentī disā sabbā, ",
  "vv18:1.6": "osadhī viya tārakā. ",
  "vv18:2.1": "Kena tetādiso vaṇṇo, ",
  "vv18:2.2": "kena te idha mijjhati; ",
  "vv18:2.3": "Uppajjanti ca te bhogā, ",
  "vv18:2.4": "ye keci manaso piyā. ",
  "vv18:3.1": "Pucchāmi taṁ devi mahānubhāve, ",
  "vv18:3.2": "Manussabhūtā kimakāsi puññaṁ; ",
  "vv18:3.3": "Kenāsi evaṁ jalitānubhāvā, ",
  "vv18:3.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti. ",
  "vv18:4.1": "Sā devatā attamanā, ",
  "vv18:4.2": "moggallānena pucchitā; ",
  "vv18:4.3": "Pañhaṁ puṭṭhā viyākāsi, ",
  "vv18:4.4": "yassa kammassidaṁ phalaṁ. ",
  "vv18:5.1": "“Ahaṁ manussesu manussabhūtā, ",
  "vv18:5.2": "Dāsī ahosiṁ parapessiyā kule. ",
  "vv18:6.1": "Upāsikā cakkhumato, ",
  "vv18:6.2": "gotamassa yasassino; ",
  "vv18:6.3": "Tassā me nikkamo āsi, ",
  "vv18:6.4": "sāsane tassa tādino. ",
  "vv18:7.1": "Kāmaṁ bhijjatuyaṁ kāyo, ",
  "vv18:7.2": "neva atthettha saṇṭhanaṁ; ",
  "vv18:7.3": "Sikkhāpadānaṁ pañcannaṁ, ",
  "vv18:7.4": "maggo sovatthiko sivo. ",
  "vv18:8.1": "Akaṇṭako agahano, ",
  "vv18:8.2": "uju sabbhi pavedito; ",
  "vv18:8.3": "Nikkamassa phalaṁ passa, ",
  "vv18:8.4": "yathidaṁ pāpuṇitthikā. ",
  "vv18:9.1": "Āmantanikā raññomhi, ",
  "vv18:9.2": "sakkassa vasavattino; ",
  "vv18:9.3": "Saṭṭhi tūriyasahassāni, ",
  "vv18:9.4": "paṭibodhaṁ karonti me. ",
  "vv18:10.1": "Ālambo gaggaro bhīmo, ",
  "vv18:10.2": "sādhuvādī ca saṁsayo; ",
  "vv18:10.3": "Pokkharo ca suphasso ca, ",
  "vv18:10.4": "viṇāmokkhā ca nāriyo. ",
  "vv18:11.1": "Nandā ceva sunandā ca, ",
  "vv18:11.2": "soṇadinnā sucimhitā; ",
  "vv18:11.3": "Alambusā missakesī ca, ",
  "vv18:11.4": "puṇḍarīkāti dāruṇī. ",
  "vv18:12.1": "Eṇīphassā suphassā ca, ",
  "vv18:12.2": "subhaddā muduvādinī; ",
  "vv18:12.3": "Etā caññā ca seyyāse, ",
  "vv18:12.4": "accharānaṁ pabodhikā. ",
  "vv18:13.1": "Tā maṁ kālenupāgantvā, ",
  "vv18:13.2": "abhibhāsanti devatā; ",
  "vv18:13.3": "Handa naccāma gāyāma, ",
  "vv18:13.4": "handa taṁ ramayāmase. ",
  "vv18:14.1": "Nayidaṁ akatapuññānaṁ, ",
  "vv18:14.2": "katapuññānamevidaṁ; ",
  "vv18:14.3": "Asokaṁ nandanaṁ rammaṁ, ",
  "vv18:14.4": "tidasānaṁ mahāvanaṁ. ",
  "vv18:15.1": "Sukhaṁ akatapuññānaṁ, ",
  "vv18:15.2": "idha natthi parattha ca; ",
  "vv18:15.3": "Sukhañca katapuññānaṁ, ",
  "vv18:15.4": "idha ceva parattha ca. ",
  "vv18:16.1": "Tesaṁ sahabyakāmānaṁ, ",
  "vv18:16.2": "kattabbaṁ kusalaṁ bahuṁ; ",
  "vv18:16.3": "Katapuññā hi modanti, ",
  "vv18:16.4": "sagge bhogasamaṅgino”ti. ",
  "vv18:17.1": "Dāsivimānaṁ paṭhamaṁ. "
}