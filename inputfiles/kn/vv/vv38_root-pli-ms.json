{
"vv38:1.1": "Vimānavatthu",
"vv38:2.1": "Itthivimāna",
"vv38:3.1": "Pāricchattakavagga",
"vv38:4.1": "10. Pāricchattakavimānavatthu",
"vv38:5.1": "“Pāricchattake koviḷāre,",
"vv38:5.2": "ramaṇīye manorame;",
"vv38:5.3": "Dibbamālaṃ ganthamānā,",
"vv38:5.4": "gāyantī sampamodasi.",
"vv38:6.1": "Tassā te naccamānāya,",
"vv38:6.2": "aṅgamaṅgehi sabbaso;",
"vv38:6.3": "Dibbā saddā niccharanti,",
"vv38:6.4": "savanīyā manoramā.",
"vv38:7.1": "Tassā te naccamānāya,",
"vv38:7.2": "aṅgamaṅgehi sabbaso;",
"vv38:7.3": "Dibbā gandhā pavāyanti,",
"vv38:7.4": "sucigandhā manoramā.",
"vv38:8.1": "Vivattamānā kāyena,",
"vv38:8.2": "yā veṇīsu piḷandhanā;",
"vv38:8.3": "Tesaṃ suyyati nigghoso,",
"vv38:8.4": "tūriye pañcaṅgike yathā.",
"vv38:9.1": "Vaṭaṃsakā vātadhutā,",
"vv38:9.2": "vātena sampakampitā;",
"vv38:9.3": "Tesaṃ suyyati nigghoso,",
"vv38:9.4": "tūriye pañcaṅgike yathā.",
"vv38:10.1": "Yāpi te sirasmiṃ mālā,",
"vv38:10.2": "sucigandhā manoramā;",
"vv38:10.3": "Vāti gandho disā sabbā,",
"vv38:10.4": "rukkho mañjūsako yathā.",
"vv38:11.1": "Ghāyase taṃ sucigandhaṃ,",
"vv38:11.2": "rūpaṃ passasi amānusaṃ;",
"vv38:11.3": "Devate pucchitācikkha,",
"vv38:11.4": "kissa kammassidaṃ phalan”ti.",
"vv38:12.1": "“Pabhassaraṃ accimantaṃ,",
"vv38:12.2": "vaṇṇagandhena saṃyutaṃ;",
"vv38:12.3": "Asokapupphamālāhaṃ,",
"vv38:12.4": "buddhassa upanāmayiṃ.",
"vv38:13.1": "Tāhaṃ kammaṃ karitvāna,",
"vv38:13.2": "kusalaṃ buddhavaṇṇitaṃ;",
"vv38:13.3": "Apetasokā sukhitā,",
"vv38:13.4": "sampamodāmanāmayā”ti.",
"vv38:14.1": "Pāricchattakavimānaṃ dasamaṃ.",
"vv38:15.1": "Tassuddānaṃ",
"vv38:16.1": "Uḷāro ucchu pallaṅko,",
"vv38:16.2": "latā ca guttilena ca;",
"vv38:16.3": "Daddallapesamallikā,",
"vv38:16.4": "visālakkhi pāricchattako;",
"vv38:16.5": "Vaggo tena pavuccatīti.",
"vv38:17.1": "Pāricchattakavaggo tatiyo."
}