{
"vv26:1.1": "Vimānavatthu",
"vv26:2.1": "Itthivimāna",
"vv26:3.1": "Cittalatāvagga",
"vv26:4.1": "9. Suniddāvimānavatthu",
"vv26:5.1": "“Abhikkantena vaṇṇena, … pe …",
"vv26:5.4": "osadhī viya tārakā.",
"vv26:6.1": "Kena tetādiso vaṇṇo, … pe …",
"vv26:6.4": "vaṇṇo ca te sabbadisā pabhāsatī”ti.",
"vv26:7.1": "Sā devatā attamanā, … pe …",
"vv26:7.4": "yassa kammassidaṃ phalaṃ.",
"vv26:8.1": "“Suniddāti maṃ aññaṃsu,",
"vv26:8.2": "rājagahasmiṃ upāsikā;",
"vv26:8.3": "Saddhā sīlena sampannā,",
"vv26:8.4": "saṃvibhāgaratā sadā.",
"vv26:9.1": "()",
"vv26:10.1": "Pañcasikkhāpade ratā,",
"vv26:10.2": "ariyasaccāna kovidā;",
"vv26:10.3": "Upāsikā cakkhumato,",
"vv26:10.4": "gotamassa yasassino.",
"vv26:11.1": "Tena metādiso vaṇṇo, … pe …",
"vv26:11.4": "vaṇṇo ca me sabbadisā pabhāsatī”ti.",
"vv26:12.1": "Suniddāvimānaṃ navamaṃ."
}