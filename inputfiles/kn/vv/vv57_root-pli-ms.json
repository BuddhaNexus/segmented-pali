{
"vv57:1.1": "Vimānavatthu",
"vv57:2.1": "Purisavimāna",
"vv57:3.1": "Mahārathavagga",
"vv57:4.1": "7. Dutiyakaraṇīyavimānavatthu",
"vv57:5.1": "“Uccamidaṃ maṇithūṇaṃ vimānaṃ,",
"vv57:5.2": "Samantato dvādasa yojanāni;",
"vv57:5.3": "Kūṭāgārā sattasatā uḷārā,",
"vv57:5.4": "Veḷuriyathambhā rucakatthatā subhā.",
"vv57:6.1": "Tatthacchasi pivasi khādasi ca,",
"vv57:6.2": "Dibbā ca vīṇā pavadanti vagguṃ;",
"vv57:6.3": "Dibbā rasā kāmaguṇettha pañca,",
"vv57:6.4": "Nāriyo ca naccanti suvaṇṇachannā.",
"vv57:7.1": "Kena tetādiso vaṇṇo, … pe …",
"vv57:7.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
"vv57:8.1": "So devaputto attamano, … pe …",
"vv57:8.4": "yassa kammassidaṃ phalaṃ.",
"vv57:9.1": "“Karaṇīyāni puññāni,",
"vv57:9.2": "paṇḍitena vijānatā;",
"vv57:9.3": "Sammaggatesu bhikkhūsu,",
"vv57:9.4": "yattha dinnaṃ mahapphalaṃ.",
"vv57:10.1": "Atthāya vata me bhikkhu,",
"vv57:10.2": "araññā gāmamāgato;",
"vv57:10.3": "Tattha cittaṃ pasādetvā,",
"vv57:10.4": "tāvatiṃsūpago ahaṃ.",
"vv57:11.1": "Tena metādiso vaṇṇo, … pe …",
"vv57:11.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
"vv57:12.1": "Dutiyakaraṇīyavimānaṃ sattamaṃ."
}