{
"vv66:1.1": "Vimānavatthu",
"vv66:2.1": "Purisavimāna",
"vv66:3.1": "Pāyāsivagga",
"vv66:4.1": "2. Dutiyaagāriyavimānavatthu",
"vv66:5.1": "“Yathā vanaṃ cittalataṃ pabhāsati,",
"vv66:5.2": "Uyyānaseṭṭhaṃ tidasānamuttamaṃ;",
"vv66:5.3": "Tathūpamaṃ tuyhamidaṃ vimānaṃ,",
"vv66:5.4": "Obhāsayaṃ tiṭṭhati antalikkhe.",
"vv66:6.1": "Deviddhipattosi mahānubhāvo,",
"vv66:6.2": "Manussabhūto kimakāsi puññaṃ;",
"vv66:6.3": "Kenāsi evaṃ jalitānubhāvo,",
"vv66:6.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
"vv66:7.1": "So devaputto attamano, … pe …",
"vv66:7.4": "yassa kammassidaṃ phalaṃ.",
"vv66:8.1": "“Ahañca bhariyā ca manussaloke,",
"vv66:8.2": "Opānabhūtā gharamāvasimha;",
"vv66:8.3": "Annañca pānañca pasannacittā,",
"vv66:8.4": "Sakkacca dānaṃ vipulaṃ adamha.",
"vv66:9.1": "Tena metādiso vaṇṇo, … pe …",
"vv66:9.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
"vv66:10.1": "Dutiyaagāriyavimānaṃ dutiyaṃ."
}