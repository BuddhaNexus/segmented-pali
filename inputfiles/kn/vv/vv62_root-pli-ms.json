{
"vv62:1.1": "Vimānavatthu",
"vv62:2.1": "Purisavimāna",
"vv62:3.1": "Mahārathavagga",
"vv62:4.1": "12. Tatiyanāgavimānavatthu",
"vv62:5.1": "“Ko nu dibbena yānena,",
"vv62:5.2": "sabbasetena hatthinā;",
"vv62:5.3": "Tūriyatāḷitanigghoso,",
"vv62:5.4": "antalikkhe mahīyati.",
"vv62:6.1": "Devatā nusi gandhabbo,",
"vv62:6.2": "adu sakko purindado;",
"vv62:6.3": "Ajānantā taṃ pucchāma,",
"vv62:6.4": "kathaṃ jānemu taṃ mayan”ti.",
"vv62:7.1": "“Namhi devo na gandhabbo,",
"vv62:7.2": "nāpi sakko purindado;",
"vv62:7.3": "Sudhammā nāma ye devā,",
"vv62:7.4": "tesaṃ aññataro ahan”ti.",
"vv62:8.1": "“Pucchāma devaṃ sudhammaṃ,",
"vv62:8.2": "Puthuṃ katvāna añjaliṃ;",
"vv62:8.3": "Kiṃ katvā mānuse kammaṃ,",
"vv62:8.4": "Sudhammaṃ upapajjatī”ti.",
"vv62:9.1": "“Ucchāgāraṃ tiṇāgāraṃ,",
"vv62:9.2": "vatthāgārañca yo dade;",
"vv62:9.3": "Tiṇṇaṃ aññataraṃ datvā,",
"vv62:9.4": "sudhammaṃ upapajjatī”ti.",
"vv62:10.1": "Tatiyanāgavimānaṃ dvādasamaṃ."
}