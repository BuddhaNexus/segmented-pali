{
"vv20:1.1": "Vimānavatthu",
"vv20:2.1": "Itthivimāna",
"vv20:3.1": "Cittalatāvagga",
"vv20:4.1": "3. Ācāmadāyikāvimānavatthu",
"vv20:5.1": "“Piṇḍāya te carantassa,",
"vv20:5.2": "tuṇhībhūtassa tiṭṭhato;",
"vv20:5.3": "Daliddā kapaṇā nārī,",
"vv20:5.4": "parāgāraṃ apassitā.",
"vv20:6.1": "Yā te adāsi ācāmaṃ,",
"vv20:6.2": "pasannā sehi pāṇibhi;",
"vv20:6.3": "Sā hitvā mānusaṃ dehaṃ,",
"vv20:6.4": "kaṃ nu sā disataṃ gatā”ti.",
"vv20:7.1": "“Piṇḍāya me carantassa,",
"vv20:7.2": "tuṇhībhūtassa tiṭṭhato;",
"vv20:7.3": "Daliddā kapaṇā nārī,",
"vv20:7.4": "parāgāraṃ apassitā.",
"vv20:8.1": "Yā me adāsi ācāmaṃ,",
"vv20:8.2": "pasannā sehi pāṇibhi;",
"vv20:8.3": "Sā hitvā mānusaṃ dehaṃ,",
"vv20:8.4": "vippamuttā ito cutā.",
"vv20:9.1": "Nimmānaratino nāma,",
"vv20:9.2": "santi devā mahiddhikā;",
"vv20:9.3": "Tattha sā sukhitā nārī,",
"vv20:9.4": "modatācāmadāyikā”ti.",
"vv20:10.1": "“Aho dānaṃ varākiyā,",
"vv20:10.2": "kassape suppatiṭṭhitaṃ;",
"vv20:10.3": "Parābhatena dānena,",
"vv20:10.4": "ijjhittha vata dakkhiṇā.",
"vv20:11.1": "Yā mahesittaṃ kāreyya,",
"vv20:11.2": "cakkavattissa rājino;",
"vv20:11.3": "Nārī sabbaṅgakalyāṇī,",
"vv20:11.4": "bhattu cānomadassikā;",
"vv20:11.5": "Etassācāmadānassa,",
"vv20:11.6": "kalaṃ nāgghati soḷasiṃ.",
"vv20:12.1": "Sataṃ nikkhā sataṃ assā,",
"vv20:12.2": "sataṃ assatarīrathā;",
"vv20:12.3": "Sataṃ kaññāsahassāni,",
"vv20:12.4": "āmuttamaṇikuṇḍalā;",
"vv20:12.5": "Etassācāmadānassa,",
"vv20:12.6": "kalaṃ nāgghanti soḷasiṃ.",
"vv20:13.1": "Sataṃ hemavatā nāgā,",
"vv20:13.2": "īsādantā urūḷhavā;",
"vv20:13.3": "Suvaṇṇakacchā mātaṅgā,",
"vv20:13.4": "hemakappanavāsasā;",
"vv20:13.5": "Etassācāmadānassa,",
"vv20:13.6": "kalaṃ nāgghati soḷasiṃ.",
"vv20:14.1": "Catunnamapi dīpānaṃ,",
"vv20:14.2": "issaraṃ yodha kāraye;",
"vv20:14.3": "Etassācāmadānassa,",
"vv20:14.4": "kalaṃ nāgghati soḷasin”ti.",
"vv20:15.1": "Ācāmadāyikāvimānaṃ tatiyaṃ."
}