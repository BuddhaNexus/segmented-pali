{
"vv12:1.1": "Vimānavatthu",
"vv12:2.1": "Itthivimāna",
"vv12:3.1": "Pīṭhavagga",
"vv12:4.1": "12. Dutiyapatibbatāvimānavatthu",
"vv12:5.1": "“Veḷuriyathambhaṃ ruciraṃ pabhassaraṃ,",
"vv12:5.2": "Vimānamāruyha anekacittaṃ;",
"vv12:5.3": "Tatthacchasi devi mahānubhāve,",
"vv12:5.4": "Uccāvacā iddhi vikubbamānā;",
"vv12:5.5": "Imā ca te accharāyo samantato,",
"vv12:5.6": "Naccanti gāyanti pamodayanti ca.",
"vv12:6.1": "Deviddhipattāsi mahānubhāve,",
"vv12:6.2": "Manussabhūtā kimakāsi puññaṃ;",
"vv12:6.3": "Kenāsi evaṃ jalitānubhāvā,",
"vv12:6.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
"vv12:7.1": "Sā devatā attamanā,",
"vv12:7.2": "moggallānena pucchitā;",
"vv12:7.3": "Pañhaṃ puṭṭhā viyākāsi,",
"vv12:7.4": "yassa kammassidaṃ phalaṃ.",
"vv12:8.1": "“Ahaṃ manussesu manussabhūtā,",
"vv12:8.2": "Upāsikā cakkhumato ahosiṃ;",
"vv12:8.3": "Pāṇātipātā viratā ahosiṃ,",
"vv12:8.4": "Loke adinnaṃ parivajjayissaṃ.",
"vv12:9.1": "Amajjapā no ca musā abhāṇiṃ,",
"vv12:9.2": "Sakena sāminā ahosiṃ tuṭṭhā;",
"vv12:9.3": "Annañca pānañca pasannacittā,",
"vv12:9.4": "Sakkacca dānaṃ vipulaṃ adāsiṃ.",
"vv12:10.1": "Tena metādiso vaṇṇo,",
"vv12:10.2": "tena me idha mijjhati;",
"vv12:10.3": "Uppajjanti ca me bhogā,",
"vv12:10.4": "ye keci manaso piyā.",
"vv12:11.1": "Akkhāmi te bhikkhu mahānubhāva,",
"vv12:11.2": "Manussabhūtā yamakāsi puññaṃ;",
"vv12:11.3": "Tenamhi evaṃ jalitānubhāvā,",
"vv12:11.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
"vv12:12.1": "Dutiyapatibbatāvimānaṃ dvādasamaṃ."
}