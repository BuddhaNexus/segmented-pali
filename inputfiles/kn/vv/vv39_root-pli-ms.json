{
  "vv39:0.1": "Vimānavatthu ",
  "vv39:0.2": "Itthivimāna ",
  "vv39:0.3": "Mañjiṭṭhakavagga ",
  "vv39:0.4": "1. Mañjiṭṭhakavimānavatthu ",
  "vv39:1.1": "“Mañjiṭṭhake vimānasmiṁ, ",
  "vv39:1.2": "soṇṇavālukasanthate; ",
  "vv39:1.3": "Pañcaṅgikena tūriyena, ",
  "vv39:1.4": "ramasi suppavādite. ",
  "vv39:2.1": "Tamhā vimānā oruyha, ",
  "vv39:2.2": "nimmitā ratanāmayā; ",
  "vv39:2.3": "Ogāhasi sālavanaṁ, ",
  "vv39:2.4": "pupphitaṁ sabbakālikaṁ. ",
  "vv39:3.1": "Yassa yasseva sālassa, ",
  "vv39:3.2": "mūle tiṭṭhasi devate; ",
  "vv39:3.3": "So so muñcati pupphāni, ",
  "vv39:3.4": "onamitvā dumuttamo. ",
  "vv39:4.1": "Vāteritaṁ sālavanaṁ, ",
  "vv39:4.2": "ādhutaṁ dijasevitaṁ; ",
  "vv39:4.3": "Vāti gandho disā sabbā, ",
  "vv39:4.4": "rukkho mañjūsako yathā. ",
  "vv39:5.1": "Ghāyase taṁ sucigandhaṁ, ",
  "vv39:5.2": "rūpaṁ passasi amānusaṁ; ",
  "vv39:5.3": "Devate pucchitācikkha, ",
  "vv39:5.4": "kissa kammassidaṁ phalan”ti. ",
  "vv39:6.1": "“Ahaṁ manussesu manussabhūtā, ",
  "vv39:6.2": "Dāsī ayirakule ahuṁ; ",
  "vv39:6.3": "Buddhaṁ nisinnaṁ disvāna, ",
  "vv39:6.4": "Sālapupphehi okiriṁ. ",
  "vv39:7.1": "Vaṭaṁsakañca sukataṁ, ",
  "vv39:7.2": "sālapupphamayaṁ ahaṁ; ",
  "vv39:7.3": "Buddhassa upanāmesiṁ, ",
  "vv39:7.4": "pasannā sehi pāṇibhi. ",
  "vv39:8.1": "Tāhaṁ kammaṁ karitvāna, ",
  "vv39:8.2": "kusalaṁ buddhavaṇṇitaṁ; ",
  "vv39:8.3": "Apetasokā sukhitā, ",
  "vv39:8.4": "sampamodāmanāmayā”ti. ",
  "vv39:9.1": "Mañjiṭṭhakavimānaṁ paṭhamaṁ. "
}