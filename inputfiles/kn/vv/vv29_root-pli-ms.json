{
"vv29:1.1": "Vimānavatthu",
"vv29:2.1": "Itthivimāna",
"vv29:3.1": "Pāricchattakavagga",
"vv29:4.1": "1. Uḷāravimānavatthu",
"vv29:5.1": "“Uḷāro te yaso vaṇṇo,",
"vv29:5.2": "sabbā obhāsate disā;",
"vv29:5.3": "Nāriyo naccanti gāyanti,",
"vv29:5.4": "devaputtā alaṅkatā.",
"vv29:6.1": "Modenti parivārenti,",
"vv29:6.2": "tava pūjāya devate;",
"vv29:6.3": "Sovaṇṇāni vimānāni,",
"vv29:6.4": "tavimāni sudassane.",
"vv29:7.1": "Tuvaṃsi issarā tesaṃ,",
"vv29:7.2": "sabbakāmasamiddhinī;",
"vv29:7.3": "Abhijātā mahantāsi,",
"vv29:7.4": "devakāye pamodasi;",
"vv29:7.5": "Devate pucchitācikkha,",
"vv29:7.6": "kissa kammassidaṃ phalan”ti.",
"vv29:8.1": "“Ahaṃ manussesu manussabhūtā,",
"vv29:8.2": "Purimāya jātiyā manussaloke;",
"vv29:8.3": "Dussīlakule suṇisā ahosiṃ,",
"vv29:8.4": "Assaddhesu kadariyesu ahaṃ.",
"vv29:9.1": "Saddhā sīlena sampannā,",
"vv29:9.2": "saṃvibhāgaratā sadā;",
"vv29:9.3": "Piṇḍāya caramānassa,",
"vv29:9.4": "apūvaṃ te adāsahaṃ.",
"vv29:10.1": "Tadāhaṃ sassuyācikkhiṃ,",
"vv29:10.2": "samaṇo āgato idha;",
"vv29:10.3": "Tassa adāsahaṃ pūvaṃ,",
"vv29:10.4": "pasannā sehi pāṇibhi.",
"vv29:11.1": "Itissā sassu paribhāsi,",
"vv29:11.2": "avinītāsi tvaṃ vadhu;",
"vv29:11.3": "Na maṃ sampucchituṃ icchi,",
"vv29:11.4": "samaṇassa dadāmahaṃ.",
"vv29:12.1": "Tato me sassu kupitā,",
"vv29:12.2": "pahāsi musalena maṃ;",
"vv29:12.3": "Kūṭaṅgacchi avadhi maṃ,",
"vv29:12.4": "nāsakkhiṃ jīvituṃ ciraṃ.",
"vv29:13.1": "Sā ahaṃ kāyassa bhedā,",
"vv29:13.2": "vippamuttā tato cutā;",
"vv29:13.3": "Devānaṃ tāvatiṃsānaṃ,",
"vv29:13.4": "upapannā sahabyataṃ.",
"vv29:14.1": "Tena metādiso vaṇṇo, … pe …",
"vv29:14.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
"vv29:15.1": "Uḷāravimānaṃ paṭhamaṃ."
}