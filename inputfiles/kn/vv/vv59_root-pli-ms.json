{
"vv59:1.1": "Vimānavatthu",
"vv59:2.1": "Purisavimāna",
"vv59:3.1": "Mahārathavagga",
"vv59:4.1": "9. Dutiyasūcivimānavatthu",
"vv59:5.1": "“Uccamidaṃ maṇithūṇaṃ vimānaṃ,",
"vv59:5.2": "Samantato dvādasa yojanāni;",
"vv59:5.3": "Kūṭāgārā sattasatā uḷārā,",
"vv59:5.4": "Veḷuriyathambhā rucakatthatā subhā.",
"vv59:6.1": "Tatthacchasi pivasi khādasi ca,",
"vv59:6.2": "Dibbā ca vīṇā pavadanti vagguṃ;",
"vv59:6.3": "Dibbā rasā kāmaguṇettha pañca,",
"vv59:6.4": "Nāriyo ca naccanti suvaṇṇachannā.",
"vv59:7.1": "Kena tetādiso vaṇṇo, … pe …",
"vv59:7.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
"vv59:8.1": "So devaputto attamano, … pe …",
"vv59:8.4": "yassa kammassidaṃ phalaṃ.",
"vv59:9.1": "“Ahaṃ manussesu manussabhūto,",
"vv59:9.2": "Purimajātiyā manussaloke.",
"vv59:10.1": "Addasaṃ virajaṃ bhikkhuṃ,",
"vv59:10.2": "vippasannamanāvilaṃ;",
"vv59:10.3": "Tassa adāsahaṃ sūciṃ,",
"vv59:10.4": "pasanno sehi pāṇibhi.",
"vv59:11.1": "Tena metādiso vaṇṇo, … pe …",
"vv59:11.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
"vv59:12.1": "Dutiyasūcivimānaṃ navamaṃ."
}