{
  "vv5:0.1": "Vimānavatthu ",
  "vv5:0.2": "Itthivimāna ",
  "vv5:0.3": "Pīṭhavagga ",
  "vv5:0.4": "5. Kuñjaravimānavatthu ",
  "vv5:1.1": "“Kuñjaro te varāroho, ",
  "vv5:1.2": "Nānāratanakappano; ",
  "vv5:1.3": "Ruciro thāmavā javasampanno, ",
  "vv5:1.4": "Ākāsamhi samīhati. ",
  "vv5:2.1": "Padumi padmapattakkhi, ",
  "vv5:2.2": "padmuppalajutindharo; ",
  "vv5:2.3": "Padmacuṇṇābhikiṇṇaṅgo, ",
  "vv5:2.4": "soṇṇapokkharamāladhā. ",
  "vv5:3.1": "Padumānusaṭaṁ maggaṁ, ",
  "vv5:3.2": "padmapattavibhūsitaṁ; ",
  "vv5:3.3": "Ṭhitaṁ vaggu manugghātī, ",
  "vv5:3.4": "mitaṁ gacchati vāraṇo. ",
  "vv5:4.1": "Tassa pakkamamānassa, ",
  "vv5:4.2": "soṇṇakaṁsā ratissarā; ",
  "vv5:4.3": "Tesaṁ suyyati nigghoso, ",
  "vv5:4.4": "tūriye pañcaṅgike yathā. ",
  "vv5:5.1": "Tassa nāgassa khandhamhi, ",
  "vv5:5.2": "sucivatthā alaṅkatā; ",
  "vv5:5.3": "Mahantaṁ accharāsaṅghaṁ, ",
  "vv5:5.4": "vaṇṇena atirocasi. ",
  "vv5:6.1": "Dānassa te idaṁ phalaṁ, ",
  "vv5:6.2": "atho sīlassa vā pana; ",
  "vv5:6.3": "Atho añjalikammassa, ",
  "vv5:6.4": "taṁ me akkhāhi pucchitā”ti. ",
  "vv5:7.1": "Sā devatā attamanā, ",
  "vv5:7.2": "moggallānena pucchitā; ",
  "vv5:7.3": "Pañhaṁ puṭṭhā viyākāsi, ",
  "vv5:7.4": "yassa kammassidaṁ phalaṁ. ",
  "vv5:8.1": "“Disvāna guṇasampannaṁ, ",
  "vv5:8.2": "jhāyiṁ jhānarataṁ sataṁ; ",
  "vv5:8.3": "Adāsiṁ pupphābhikiṇṇaṁ, ",
  "vv5:8.4": "āsanaṁ dussasanthataṁ. ",
  "vv5:9.1": "Upaḍḍhaṁ padmamālāhaṁ, ",
  "vv5:9.2": "āsanassa samantato; ",
  "vv5:9.3": "Abbhokirissaṁ pattehi, ",
  "vv5:9.4": "pasannā sehi pāṇibhi. ",
  "vv5:10.1": "Tassa kammakusalassa, ",
  "vv5:10.2": "idaṁ me īdisaṁ phalaṁ; ",
  "vv5:10.3": "Sakkāro garukāro ca, ",
  "vv5:10.4": "devānaṁ apacitā ahaṁ. ",
  "vv5:11.1": "Yo ve sammāvimuttānaṁ, ",
  "vv5:11.2": "santānaṁ brahmacārinaṁ; ",
  "vv5:11.3": "Pasanno āsanaṁ dajjā, ",
  "vv5:11.4": "evaṁ nande yathā ahaṁ. ",
  "vv5:12.1": "Tasmā hi attakāmena, ",
  "vv5:12.2": "mahattamabhikaṅkhatā; ",
  "vv5:12.3": "Āsanaṁ dātabbaṁ hoti, ",
  "vv5:12.4": "sarīrantimadhārinan”ti. ",
  "vv5:13.1": "Kuñjaravimānaṁ pañcamaṁ. "
}