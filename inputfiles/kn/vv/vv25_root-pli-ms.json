{
"vv25:1.1": "Vimānavatthu",
"vv25:2.1": "Itthivimāna",
"vv25:3.1": "Cittalatāvagga",
"vv25:4.1": "8. Niddāvimānavatthu",
"vv25:5.1": "“Abhikkantena vaṇṇena,",
"vv25:5.2": "yā tvaṃ tiṭṭhasi devate;",
"vv25:5.3": "Obhāsentī disā sabbā,",
"vv25:5.4": "osadhī viya tārakā.",
"vv25:6.1": "Kena tetādiso vaṇṇo, … pe …",
"vv25:6.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
"vv25:7.1": "Sā devatā attamanā, … pe …",
"vv25:7.4": "yassa kammassidaṃ phalaṃ.",
"vv25:8.1": "“Niddāti mamaṃ aññaṃsu,",
"vv25:8.2": "Rājagahasmiṃ upāsikā;",
"vv25:8.3": "Saddhā sīlena sampannā,",
"vv25:8.4": "Saṃvibhāgaratā sadā.",
"vv25:9.1": "Acchādanañca bhattañca,",
"vv25:9.2": "senāsanaṃ padīpiyaṃ;",
"vv25:9.3": "Adāsiṃ ujubhūtesu,",
"vv25:9.4": "vippasannena cetasā.",
"vv25:10.1": "Cātuddasiṃ pañcadasiṃ,",
"vv25:10.2": "yā ca pakkhassa aṭṭhamī;",
"vv25:10.3": "Pāṭihāriyapakkhañca,",
"vv25:10.4": "aṭṭhaṅgasusamāgataṃ.",
"vv25:11.1": "Uposathaṃ upavasissaṃ,",
"vv25:11.2": "sadā sīlesu saṃvutā;",
"vv25:11.3": "Saññamā saṃvibhāgā ca,",
"vv25:11.4": "vimānaṃ āvasāmahaṃ.",
"vv25:12.1": "Pāṇātipātā viratā,",
"vv25:12.2": "musāvādā ca saññatā;",
"vv25:12.3": "Theyyā ca aticārā ca,",
"vv25:12.4": "majjapānā ca ārakā.",
"vv25:13.1": "Pañcasikkhāpade ratā,",
"vv25:13.2": "ariyasaccāna kovidā;",
"vv25:13.3": "Upāsikā cakkhumato,",
"vv25:13.4": "gotamassa yasassino.",
"vv25:14.1": "Tena metādiso vaṇṇo, … pe …",
"vv25:14.4": "vaṇṇo ca me sabbadisā pabhāsatī”ti.",
"vv25:15.1": "Niddāvimānaṃ aṭṭhamaṃ."
}