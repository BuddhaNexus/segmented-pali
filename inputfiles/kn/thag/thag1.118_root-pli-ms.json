{
  "thag1.118:0.1": "Theragāthā",
  "thag1.118:0.2": "Ekakanipāta",
  "thag1.118:0.3": "Dvādasamavagga",
  "thag1.118:0.4": "1.118 Kimilattheragāthā",
  "thag1.118:1.1": "“Abhisattova nipatati,",
  "thag1.118:1.2": "Vayo rūpaṁ aññamiva tatheva santaṁ;",
  "thag1.118:1.3": "Tasseva sato avippavasato,",
  "thag1.118:1.4": "Aññasseva sarāmi attānan”ti.",
  "thag1.118:1.5": "… Kimilo thero …."
}