{
  "thag1.114:0.1": "Theragāthā",
  "thag1.114:0.2": "Ekakanipāta",
  "thag1.114:0.3": "Dvādasamavagga",
  "thag1.114:0.4": "1.114. Adhimuttattheragāthā",
  "thag1.114:1.1": "“Kāyaduṭṭhullagaruno,",
  "thag1.114:1.2": "hiyyamānamhi jīvite;",
  "thag1.114:1.3": "Sarīrasukhagiddhassa,",
  "thag1.114:1.4": "kuto samaṇasādhutā”ti.",
  "thag1.114:1.5": "… Adhimutto thero …."
}