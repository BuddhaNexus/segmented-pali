{
  "cp26:0.1": "Cariyāpiṭaka ",
  "cp26:0.2": "Yudhañjayavagga ",
  "cp26:0.3": "Adiṭṭhānapāramī 1 ",
  "cp26:0.4": "Temiyacariya ",
  "cp26:1.1": "“Punāparaṁ yadā homi, ",
  "cp26:1.2": "kāsirājassa atrajo; ",
  "cp26:1.3": "Mūgapakkhoti nāmena, ",
  "cp26:1.4": "temiyoti vadanti maṁ. ",
  "cp26:2.1": "Soḷasitthisahassānaṁ, ",
  "cp26:2.2": "na vijjati pumo tadā; ",
  "cp26:2.3": "Ahorattānaṁ accayena, ",
  "cp26:2.4": "nibbatto ahamekako. ",
  "cp26:3.1": "Kicchā laddhaṁ piyaṁ puttaṁ, ",
  "cp26:3.2": "abhijātaṁ jutindharaṁ; ",
  "cp26:3.3": "Setacchattaṁ dhārayitvāna, ",
  "cp26:3.4": "sayane poseti maṁ pitā. ",
  "cp26:4.1": "Niddāyamāno sayanavare, ",
  "cp26:4.2": "Pabujjhitvānahaṁ tadā; ",
  "cp26:4.3": "Addasaṁ paṇḍaraṁ chattaṁ, ",
  "cp26:4.4": "Yenāhaṁ nirayaṁ gato. ",
  "cp26:5.1": "Saha diṭṭhassa me chattaṁ, ",
  "cp26:5.2": "tāso uppajji bheravo; ",
  "cp26:5.3": "Vinicchayaṁ samāpanno, ",
  "cp26:5.4": "‘kathāhaṁ imaṁ muñcissaṁ’. ",
  "cp26:6.1": "Pubbasālohitā mayhaṁ, ",
  "cp26:6.2": "devatā atthakāminī; ",
  "cp26:6.3": "Sā maṁ disvāna dukkhitaṁ, ",
  "cp26:6.4": "tīsu ṭhānesu yojayi. ",
  "cp26:7.1": "‘Mā paṇḍiccayaṁ vibhāvaya, ",
  "cp26:7.2": "Bālamato bhava sabbapāṇinaṁ; ",
  "cp26:7.3": "Sabbo taṁ jano ocināyatu, ",
  "cp26:7.4": "Evaṁ tava attho bhavissati’. ",
  "cp26:8.1": "Evaṁ vuttāyahaṁ tassā, ",
  "cp26:8.2": "idaṁ vacanamabraviṁ; ",
  "cp26:8.3": "‘Karomi te taṁ vacanaṁ, ",
  "cp26:8.4": "yaṁ tvaṁ bhaṇasi devate; ",
  "cp26:8.5": "Atthakāmāsi me amma, ",
  "cp26:8.6": "hitakāmāsi devate’. ",
  "cp26:9.1": "Tassāhaṁ vacanaṁ sutvā, ",
  "cp26:9.2": "sāgareva thalaṁ labhiṁ; ",
  "cp26:9.3": "Haṭṭho saṁviggamānaso, ",
  "cp26:9.4": "tayo aṅge adhiṭṭhahiṁ. ",
  "cp26:10.1": "Mūgo ahosiṁ badhiro, ",
  "cp26:10.2": "pakkho gativivajjito; ",
  "cp26:10.3": "Ete aṅge adhiṭṭhāya, ",
  "cp26:10.4": "vassāni soḷasaṁ vasiṁ. ",
  "cp26:11.1": "Tato me hatthapāde ca, ",
  "cp26:11.2": "jivhaṁ sotañca maddiya; ",
  "cp26:11.3": "Anūnataṁ me passitvā, ",
  "cp26:11.4": "‘kāḷakaṇṇī’ti nindisuṁ. ",
  "cp26:12.1": "Tato jānapadā sabbe, ",
  "cp26:12.2": "senāpatipurohitā; ",
  "cp26:12.3": "Sabbe ekamanā hutvā, ",
  "cp26:12.4": "chaḍḍanaṁ anumodisuṁ. ",
  "cp26:13.1": "Sohaṁ tesaṁ matiṁ sutvā, ",
  "cp26:13.2": "haṭṭho saṁviggamānaso; ",
  "cp26:13.3": "‘Yassatthāya tapociṇṇo, ",
  "cp26:13.4": "so me attho samijjhatha’. ",
  "cp26:14.1": "Nhāpetvā anulimpitvā, ",
  "cp26:14.2": "veṭhetvā rājaveṭhanaṁ; ",
  "cp26:14.3": "Chattena abhisiñcitvā, ",
  "cp26:14.4": "kāresuṁ puraṁ padakkhiṇaṁ. ",
  "cp26:15.1": "Sattāhaṁ dhārayitvāna, ",
  "cp26:15.2": "uggate ravimaṇḍale; ",
  "cp26:15.3": "Rathena maṁ nīharitvā, ",
  "cp26:15.4": "sārathī vanamupāgami. ",
  "cp26:16.1": "Ekokāse rathaṁ katvā, ",
  "cp26:16.2": "sajjassaṁ hatthamuccito; ",
  "cp26:16.3": "Sārathī khaṇatī kāsuṁ, ",
  "cp26:16.4": "nikhātuṁ pathaviyā mamaṁ. ",
  "cp26:17.1": "Adhiṭṭhitamadhiṭṭhānaṁ, ",
  "cp26:17.2": "tajjento vividhakāraṇā; ",
  "cp26:17.3": "Na bhindiṁ tamadhiṭṭhānaṁ, ",
  "cp26:17.4": "bodhiyāyeva kāraṇā. ",
  "cp26:18.1": "Mātāpitā na me dessā, ",
  "cp26:18.2": "attā me na ca dessiyo; ",
  "cp26:18.3": "Sabbaññutaṁ piyaṁ mayhaṁ, ",
  "cp26:18.4": "tasmā vatamadhiṭṭhahiṁ. ",
  "cp26:19.1": "Ete aṅge adhiṭṭhāya, ",
  "cp26:19.2": "Vassāni soḷasaṁ vasiṁ; ",
  "cp26:19.3": "Adhiṭṭhānena me samo natthi, ",
  "cp26:19.4": "Esā me adhiṭṭhānapāramī”ti. ",
  "cp26:20.1": "Temiyacariyaṁ chaṭṭhaṁ. "
}