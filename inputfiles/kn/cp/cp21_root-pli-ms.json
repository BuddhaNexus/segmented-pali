{
  "cp21:0.1": "Cariyāpiṭaka ",
  "cp21:0.2": "Yudhañjayavagga ",
  "cp21:0.3": "Nekkhammapāramī 1 ",
  "cp21:0.4": "Yudhañjayacariya ",
  "cp21:1.1": "“Yadāhaṁ amitayaso, ",
  "cp21:1.2": "Rājaputto yudhañjayo; ",
  "cp21:1.3": "Ussāvabinduṁ sūriyātape, ",
  "cp21:1.4": "Patitaṁ disvāna saṁvijiṁ. ",
  "cp21:2.1": "Taññevādhipatiṁ katvā, ",
  "cp21:2.2": "saṁvegamanubrūhayiṁ; ",
  "cp21:2.3": "Mātāpitū ca vanditvā, ",
  "cp21:2.4": "pabbajjamanuyācahaṁ. ",
  "cp21:3.1": "Yācanti maṁ pañjalikā, ",
  "cp21:3.2": "sanegamā saraṭṭhakā; ",
  "cp21:3.3": "‘Ajjeva putta paṭipajja, ",
  "cp21:3.4": "iddhaṁ phītaṁ mahāmahiṁ’. ",
  "cp21:4.1": "Sarājake sahorodhe, ",
  "cp21:4.2": "sanegame saraṭṭhake; ",
  "cp21:4.3": "Karuṇaṁ paridevante, ",
  "cp21:4.4": "anapekkhova pariccajiṁ. ",
  "cp21:5.1": "Kevalaṁ pathaviṁ rajjaṁ, ",
  "cp21:5.2": "ñātiparijanaṁ yasaṁ; ",
  "cp21:5.3": "Cajamāno na cintesiṁ, ",
  "cp21:5.4": "bodhiyāyeva kāraṇā. ",
  "cp21:6.1": "Mātāpitā na me dessā, ",
  "cp21:6.2": "napi me dessaṁ mahāyasaṁ; ",
  "cp21:6.3": "Sabbaññutaṁ piyaṁ mayhaṁ, ",
  "cp21:6.4": "tasmā rajjaṁ pariccajin”ti. ",
  "cp21:7.1": "Yudhañjayacariyaṁ paṭhamaṁ. "
}