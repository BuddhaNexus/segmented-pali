{
"cp23:1.1": "Cariyāpiṭaka",
"cp23:2.1": "Yudhañjayavagga",
"cp23:3.1": "Nekkhammapāramī 3",
"cp23:4.1": "Ayogharacariya",
"cp23:5.1": "“Punāparaṃ yadā homi,",
"cp23:5.2": "kāsirājassa atrajo;",
"cp23:5.3": "Ayogharamhi saṃvaḍḍho,",
"cp23:5.4": "nāmenāsi ayogharo.",
"cp23:6.1": "‘Dukkhena jīvito laddho,",
"cp23:6.2": "saṃpīḷe patiposito;",
"cp23:6.3": "Ajjeva putta paṭipajja,",
"cp23:6.4": "kevalaṃ vasudhaṃ imaṃ’.",
"cp23:7.1": "Saraṭṭhakaṃ sanigamaṃ,",
"cp23:7.2": "sajanaṃ vanditva khattiyaṃ;",
"cp23:7.3": "Añjaliṃ paggahetvāna,",
"cp23:7.4": "idaṃ vacanamabraviṃ.",
"cp23:8.1": "‘Ye keci mahiyā sattā,",
"cp23:8.2": "hīnamukkaṭṭhamajjhimā;",
"cp23:8.3": "Nirārakkhā sake gehe,",
"cp23:8.4": "vaḍḍhanti sakañātibhi.",
"cp23:9.1": "Idaṃ loke uttariyaṃ,",
"cp23:9.2": "saṃpīḷe mama posanaṃ;",
"cp23:9.3": "Ayogharamhi saṃvaḍḍho,",
"cp23:9.4": "appabhe candasūriye.",
"cp23:10.1": "Pūtikuṇapasampuṇṇā,",
"cp23:10.2": "muccitvā mātukucchito;",
"cp23:10.3": "Tato ghoratare dukkhe,",
"cp23:10.4": "puna pakkhittayoghare.",
"cp23:11.1": "Yadihaṃ tādisaṃ patvā,",
"cp23:11.2": "dukkhaṃ paramadāruṇaṃ;",
"cp23:11.3": "Rajjesu yadi rajjāmi,",
"cp23:11.4": "pāpānaṃ uttamo siyaṃ.",
"cp23:12.1": "Ukkaṇṭhitomhi kāyena,",
"cp23:12.2": "rajjenamhi anatthiko;",
"cp23:12.3": "Nibbutiṃ pariyesissaṃ,",
"cp23:12.4": "yattha maṃ maccu na maddiye’.",
"cp23:13.1": "Evāhaṃ cintayitvāna,",
"cp23:13.2": "viravante mahājane;",
"cp23:13.3": "Nāgova bandhanaṃ chetvā,",
"cp23:13.4": "pāvisiṃ kānanaṃ vanaṃ.",
"cp23:14.1": "Mātāpitā na me dessā,",
"cp23:14.2": "napi me dessaṃ mahāyasaṃ;",
"cp23:14.3": "Sabbaññutaṃ piyaṃ mayhaṃ,",
"cp23:14.4": "tasmā rajjaṃ pariccajin”ti.",
"cp23:15.1": "Ayogharacariyaṃ tatiyaṃ."
}