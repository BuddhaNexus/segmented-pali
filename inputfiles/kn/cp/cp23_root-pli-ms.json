{
  "cp23:0.1": "Cariyāpiṭaka ",
  "cp23:0.2": "Yudhañjayavagga ",
  "cp23:0.3": "Nekkhammapāramī 3 ",
  "cp23:0.4": "Ayogharacariya ",
  "cp23:1.1": "“Punāparaṁ yadā homi, ",
  "cp23:1.2": "kāsirājassa atrajo; ",
  "cp23:1.3": "Ayogharamhi saṁvaḍḍho, ",
  "cp23:1.4": "nāmenāsi ayogharo. ",
  "cp23:2.1": "‘Dukkhena jīvito laddho, ",
  "cp23:2.2": "saṁpīḷe patiposito; ",
  "cp23:2.3": "Ajjeva putta paṭipajja, ",
  "cp23:2.4": "kevalaṁ vasudhaṁ imaṁ’. ",
  "cp23:3.1": "Saraṭṭhakaṁ sanigamaṁ, ",
  "cp23:3.2": "sajanaṁ vanditva khattiyaṁ; ",
  "cp23:3.3": "Añjaliṁ paggahetvāna, ",
  "cp23:3.4": "idaṁ vacanamabraviṁ. ",
  "cp23:4.1": "‘Ye keci mahiyā sattā, ",
  "cp23:4.2": "hīnamukkaṭṭhamajjhimā; ",
  "cp23:4.3": "Nirārakkhā sake gehe, ",
  "cp23:4.4": "vaḍḍhanti sakañātibhi. ",
  "cp23:5.1": "Idaṁ loke uttariyaṁ, ",
  "cp23:5.2": "saṁpīḷe mama posanaṁ; ",
  "cp23:5.3": "Ayogharamhi saṁvaḍḍho, ",
  "cp23:5.4": "appabhe candasūriye. ",
  "cp23:6.1": "Pūtikuṇapasampuṇṇā, ",
  "cp23:6.2": "muccitvā mātukucchito; ",
  "cp23:6.3": "Tato ghoratare dukkhe, ",
  "cp23:6.4": "puna pakkhittayoghare. ",
  "cp23:7.1": "Yadihaṁ tādisaṁ patvā, ",
  "cp23:7.2": "dukkhaṁ paramadāruṇaṁ; ",
  "cp23:7.3": "Rajjesu yadi rajjāmi, ",
  "cp23:7.4": "pāpānaṁ uttamo siyaṁ. ",
  "cp23:8.1": "Ukkaṇṭhitomhi kāyena, ",
  "cp23:8.2": "rajjenamhi anatthiko; ",
  "cp23:8.3": "Nibbutiṁ pariyesissaṁ, ",
  "cp23:8.4": "yattha maṁ maccu na maddiye’. ",
  "cp23:9.1": "Evāhaṁ cintayitvāna, ",
  "cp23:9.2": "viravante mahājane; ",
  "cp23:9.3": "Nāgova bandhanaṁ chetvā, ",
  "cp23:9.4": "pāvisiṁ kānanaṁ vanaṁ. ",
  "cp23:10.1": "Mātāpitā na me dessā, ",
  "cp23:10.2": "napi me dessaṁ mahāyasaṁ; ",
  "cp23:10.3": "Sabbaññutaṁ piyaṁ mayhaṁ, ",
  "cp23:10.4": "tasmā rajjaṁ pariccajin”ti. ",
  "cp23:11.1": "Ayogharacariyaṁ tatiyaṁ. "
}