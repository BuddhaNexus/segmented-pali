{
  "cp28:0.1": "Cariyāpiṭaka",
  "cp28:0.2": "Yudhañjayavagga",
  "cp28:0.3": "Saccapāramī 2",
  "cp28:0.4": "Saccatāpasacariya",
  "cp28:1.1": "“Punāparaṁ yadā homi,",
  "cp28:1.2": "Tāpaso saccasavhayo;",
  "cp28:1.3": "Saccena lokaṁ pālesiṁ,",
  "cp28:1.4": "Samaggaṁ janamakāsahan”ti.",
  "cp28:2.1": "Saccatāpasacariyaṁ aṭṭhamaṁ."
}