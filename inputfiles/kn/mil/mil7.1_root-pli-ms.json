{
"mil7.1:1.1": "Milindapañha",
"mil7.1:2.1": "7 Opammakathāpañha",
"mil7.1:3.1": "Mātikā",
"mil7.1:4.1": "“Bhante nāgasena, katihaṅgehi samannāgato bhikkhu arahattaṃ sacchikarotī”ti?",
"mil7.1:5.1": "“Idha, mahārāja, arahattaṃ sacchikātukāmena bhikkhunā—",
"mil7.1:6.1": "Gadrabhassa ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:7.1": "Kukkuṭassa pañca aṅgāni gahetabbāni.",
"mil7.1:8.1": "Kalandakassa ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:9.1": "Dīpiniyā ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:10.1": "Dīpikassa dve aṅgāni gahetabbāni.",
"mil7.1:11.1": "Kummassa pañca aṅgāni gahetabbāni.",
"mil7.1:12.1": "Vaṃsassa ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:13.1": "Cāpassa ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:14.1": "Vāyasassa dve aṅgāni gahetabbāni.",
"mil7.1:15.1": "Makkaṭassa dve aṅgāni gahetabbāni.",
"mil7.1:16.1": "Gadrabhavaggo paṭhamo.",
"mil7.1:17.1": "Lābulatāya ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:18.1": "Padumassa tīṇi aṅgāni gahetabbāni.",
"mil7.1:19.1": "Bījassa dve aṅgāni gahetabbāni.",
"mil7.1:20.1": "Sālakalyāṇikāya ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:21.1": "Nāvāya tīṇi aṅgāni gahetabbāni.",
"mil7.1:22.1": "Nāvālagganakassa dve aṅgāni gahetabbāni.",
"mil7.1:23.1": "Kūpassa ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:24.1": "Niyāmakassa tīṇi aṅgāni gahetabbāni.",
"mil7.1:25.1": "Kammakārassa ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:26.1": "Samuddassa pañca aṅgāni gahetabbāni.",
"mil7.1:27.1": "Samuddavaggo dutiyo.",
"mil7.1:28.1": "Pathaviyā pañca aṅgāni gahetabbāni.",
"mil7.1:29.1": "Āpassa pañca aṅgāni gahetabbāni.",
"mil7.1:30.1": "Tejassa pañca aṅgāni gahetabbāni.",
"mil7.1:31.1": "Vāyussa pañca aṅgāni gahetabbāni.",
"mil7.1:32.1": "Pabbatassa pañca aṅgāni gahetabbāni.",
"mil7.1:33.1": "Ākāsassa pañca aṅgāni gahetabbāni.",
"mil7.1:34.1": "Candassa pañca aṅgāni gahetabbāni.",
"mil7.1:35.1": "Sūriyassa satta aṅgāni gahetabbāni.",
"mil7.1:36.1": "Sakkassa tīṇi aṅgāni gahetabbāni.",
"mil7.1:37.1": "Cakkavattissa cattāri aṅgāni gahetabbāni.",
"mil7.1:38.1": "Pathavīvaggo tatiyo.",
"mil7.1:39.1": "Upacikāya ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:40.1": "Biḷārassa dve aṅgāni gahetabbāni.",
"mil7.1:41.1": "Undūrassa ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:42.1": "Vicchikassa ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:43.1": "Nakulassa ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:44.1": "Jarasiṅgālassa dve aṅgāni gahetabbāni.",
"mil7.1:45.1": "Migassa tīṇi aṅgāni gahetabbāni.",
"mil7.1:46.1": "Gorūpassa cattāri aṅgāni gahetabbāni.",
"mil7.1:47.1": "Varāhassa dve aṅgāni gahetabbāni.",
"mil7.1:48.1": "Hatthissa pañca aṅgāni gahetabbāni.",
"mil7.1:49.1": "Upacikāvaggo catuttho.",
"mil7.1:50.1": "Sīhassa satta aṅgāni gahetabbāni.",
"mil7.1:51.1": "Cakkavākassa tīṇi aṅgāni gahetabbāni.",
"mil7.1:52.1": "Peṇāhikāya dve aṅgāni gahetabbāni.",
"mil7.1:53.1": "Gharakapotassa ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:54.1": "Ulūkassa dve aṅgāni gahetabbāni.",
"mil7.1:55.1": "Satapattassa ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:56.1": "Vaggulissa dve aṅgāni gahetabbāni.",
"mil7.1:57.1": "Jalūkāya ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:58.1": "Sappassa tīṇi aṅgāni gahetabbāni.",
"mil7.1:59.1": "Ajagarassa ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:60.1": "Sīhavaggo pañcamo.",
"mil7.1:61.1": "Panthamakkaṭakassa ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:62.1": "Thanasitadārakassa ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:63.1": "Cittakadharakummassa ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:64.1": "Pavanassa pañca aṅgāni gahetabbāni.",
"mil7.1:65.1": "Rukkhassa tīṇi aṅgāni gahetabbāni.",
"mil7.1:66.1": "Meghassa pañca aṅgāni gahetabbāni.",
"mil7.1:67.1": "Maṇiratanassa tīṇi aṅgāni gahetabbāni.",
"mil7.1:68.1": "Māgavikassa cattāri aṅgāni gahetabbāni.",
"mil7.1:69.1": "Bāḷisikassa dve aṅgāni gahetabbāni.",
"mil7.1:70.1": "Tacchakassa dve aṅgāni gahetabbāni.",
"mil7.1:71.1": "Makkaṭavaggo chaṭṭho.",
"mil7.1:72.1": "Kumbhassa ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:73.1": "Kāḷāyasassa dve aṅgāni gahetabbāni.",
"mil7.1:74.1": "Chattassa tīṇi aṅgāni gahetabbāni.",
"mil7.1:75.1": "Khettassa tīṇi aṅgāni gahetabbāni.",
"mil7.1:76.1": "Agadassa dve aṅgāni gahetabbāni.",
"mil7.1:77.1": "Bhojanassa tīṇi aṅgāni gahetabbāni.",
"mil7.1:78.1": "Issāsassa cattāri aṅgāni gahetabbāni.",
"mil7.1:79.1": "Kumbhavaggo sattamo.",
"mil7.1:80.1": "Rañño cattāri aṅgāni gahetabbāni.",
"mil7.1:81.1": "Dovārikassa dve aṅgāni gahetabbāni.",
"mil7.1:82.1": "Nisadāya ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:83.1": "Padīpassa dve aṅgāni gahetabbāni.",
"mil7.1:84.1": "Mayūrassa dve aṅgāni gahetabbāni.",
"mil7.1:85.1": "Turaṅgassa dve aṅgāni gahetabbāni.",
"mil7.1:86.1": "Soṇḍikassa dve aṅgāni gahetabbāni.",
"mil7.1:87.1": "Indakhīlassa dve aṅgāni gahetabbāni.",
"mil7.1:88.1": "Tulāya ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:89.1": "Khaggassa dve aṅgāni gahetabbāni.",
"mil7.1:90.1": "Macchassa dve aṅgāni gahetabbāni.",
"mil7.1:91.1": "Iṇaggāhakassa ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:92.1": "Byādhitassa dve aṅgāni gahetabbāni.",
"mil7.1:93.1": "Matassa dve aṅgāni gahetabbāni.",
"mil7.1:94.1": "Nadiyā dve aṅgāni gahetabbāni.",
"mil7.1:95.1": "Usabhassa ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:96.1": "Maggassa dve aṅgāni gahetabbāni.",
"mil7.1:97.1": "Suṅkasāyikassa ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:98.1": "Corassa tīṇi aṅgāni gahetabbāni.",
"mil7.1:99.1": "Sakuṇagghiyā ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:100.1": "Sunakhassa ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:101.1": "Tikicchakassa tīṇi aṅgāni gahetabbāni.",
"mil7.1:102.1": "Gabbhiniyā dve aṅgāni gahetabbāni.",
"mil7.1:103.1": "Camariyā ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:104.1": "Kikiyā dve aṅgāni gahetabbāni.",
"mil7.1:105.1": "Kapotikāya tīṇi aṅgāni gahetabbāni.",
"mil7.1:106.1": "Ekanayanassa dve aṅgāni gahetabbāni.",
"mil7.1:107.1": "Kassakassa tīṇi aṅgāni gahetabbāni.",
"mil7.1:108.1": "Jambukasiṅgāliyā ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:109.1": "Caṅgavārakassa dve aṅgāni gahetabbāni.",
"mil7.1:110.1": "Dabbiyā ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:111.1": "Iṇasādhakassa tīṇi aṅgāni gahetabbāni.",
"mil7.1:112.1": "Anuvicinakassa ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:113.1": "Sārathissa dve aṅgāni gahetabbāni.",
"mil7.1:114.1": "Bhojakassa dve aṅgāni gahetabbāni.",
"mil7.1:115.1": "Tunnavāyassa ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:116.1": "Nāvikassa ekaṃ aṅgaṃ gahetabbaṃ.",
"mil7.1:117.1": "Bhamarassa dve aṅgāni gahetabbānī”ti.",
"mil7.1:118.1": "Mātikā niṭṭhitā."
}