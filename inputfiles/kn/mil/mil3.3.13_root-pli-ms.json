{
"mil3.3.13:1.1": "Milindapañha",
"mil3.3.13:2.1": "Vicāravagga",
"mil3.3.13:3.1": "13. Vitakkalakkhaṇapañha",
"mil3.3.13:4.1": "“Bhante nāgasena, kiṃlakkhaṇo vitakko”ti?",
"mil3.3.13:4.2": "“Appanālakkhaṇo, mahārāja, vitakko”ti.",
"mil3.3.13:5.1": "“Opammaṃ karohī”ti.",
"mil3.3.13:5.2": "“Yathā, mahārāja, vaḍḍhakī suparikammakataṃ dāruṃ sandhismiṃ appeti;",
"mil3.3.13:5.3": "evameva kho, mahārāja, appanālakkhaṇo vitakko”ti.",
"mil3.3.13:6.1": "“Kallosi, bhante nāgasenā”ti.",
"mil3.3.13:7.1": "Vitakkalakkhaṇapañho terasamo."
}