{
"mil3.6.8:1.1": "Milindapañha",
"mil3.6.8:2.1": "Sativagga",
"mil3.6.8:3.1": "8. Paññāpatiṭṭhānapañha",
"mil3.6.8:4.1": "Rājā āha—",
"mil3.6.8:4.2": "“bhante nāgasena, paññā kuhiṃ paṭivasatī”ti?",
"mil3.6.8:4.3": "“Na katthaci, mahārājā”ti.",
"mil3.6.8:4.4": "“Tena hi, bhante nāgasena, natthi paññā”ti.",
"mil3.6.8:4.5": "“Vāto, mahārāja, kuhiṃ paṭivasatī”ti?",
"mil3.6.8:4.6": "“Na katthaci, bhante”ti.",
"mil3.6.8:4.7": "“Tena hi, mahārāja, natthi vāto”ti.",
"mil3.6.8:5.1": "“Kallosi, bhante nāgasenā”ti.",
"mil3.6.8:6.1": "Paññāpatiṭṭhānapañho aṭṭhamo."
}