{
  "mil3.6.8:0.1": "Milindapañha ",
  "mil3.6.8:0.2": "Sativagga ",
  "mil3.6.8:0.3": "8. Paññāpatiṭṭhānapañha ",
  "mil3.6.8:1.1": "Rājā āha—",
  "mil3.6.8:1.2": "“bhante nāgasena, paññā kuhiṁ paṭivasatī”ti? ",
  "mil3.6.8:1.3": "“Na katthaci, mahārājā”ti. ",
  "mil3.6.8:1.4": "“Tena hi, bhante nāgasena, natthi paññā”ti. ",
  "mil3.6.8:1.5": "“Vāto, mahārāja, kuhiṁ paṭivasatī”ti? ",
  "mil3.6.8:1.6": "“Na katthaci, bhante”ti. ",
  "mil3.6.8:1.7": "“Tena hi, mahārāja, natthi vāto”ti. ",
  "mil3.6.8:2.1": "“Kallosi, bhante nāgasenā”ti. ",
  "mil3.6.8:3.1": "Paññāpatiṭṭhānapañho aṭṭhamo. "
}