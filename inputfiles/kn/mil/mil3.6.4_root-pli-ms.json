{
"mil3.6.4:1.1": "Milindapañha",
"mil3.6.4:2.1": "Sativagga",
"mil3.6.4:3.1": "4. Bhagavatobrahmacāripañha",
"mil3.6.4:4.1": "Rājā āha—",
"mil3.6.4:4.2": "“bhante nāgasena, buddho brahmacārī”ti?",
"mil3.6.4:4.3": "“Āma, mahārāja, bhagavā brahmacārī”ti.",
"mil3.6.4:4.4": "“Tena hi, bhante nāgasena, buddho brahmuno sisso”ti?",
"mil3.6.4:4.5": "“Atthi pana te, mahārāja, hatthipāmokkho”ti?",
"mil3.6.4:4.6": "“Atthi, bhante”ti.",
"mil3.6.4:4.7": "“Kiṃ nu kho, mahārāja, so hatthī kadāci karahaci koñcanādaṃ nadatī”ti?",
"mil3.6.4:4.8": "“Āma, bhante, nadatī”ti.",
"mil3.6.4:4.9": "“Tena hi, mahārāja, so hatthī koñcasakuṇassa sisso”ti?",
"mil3.6.4:4.10": "“Na hi, bhante”ti.",
"mil3.6.4:4.11": "“Kiṃ pana, mahārāja, brahmā sabuddhiko abuddhiko”ti?",
"mil3.6.4:4.12": "“Sabuddhiko, bhante”ti.",
"mil3.6.4:4.13": "“Tena hi, mahārāja, brahmā bhagavato sisso”ti.",
"mil3.6.4:5.1": "“Kallosi, bhante nāgasenā”ti.",
"mil3.6.4:6.1": "Bhagavato brahmacāripañho catuttho."
}