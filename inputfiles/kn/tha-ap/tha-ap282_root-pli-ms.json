{
"tha-ap282:1.1": "Therāpadāna",
"tha-ap282:2.1": "Suvaṇṇabibbohanavagga",
"tha-ap282:3.1": "10 Taraṇīyattheraapadāna",
"tha-ap282:4.1": "“Mahāpathamhi visame,",
"tha-ap282:4.2": "setu kārāpito mayā;",
"tha-ap282:4.3": "Taraṇatthāya lokassa,",
"tha-ap282:4.4": "pasanno sehi pāṇibhi.",
"tha-ap282:5.1": "Ekanavutito kappe,",
"tha-ap282:5.2": "yo setu kārito mayā;",
"tha-ap282:5.3": "Duggatiṃ nābhijānāmi,",
"tha-ap282:5.4": "setudānassidaṃ phalaṃ.",
"tha-ap282:6.1": "Pañcapaññāsito kappe,",
"tha-ap282:6.2": "eko āsiṃ samogadho;",
"tha-ap282:6.3": "Sattaratanasampanno,",
"tha-ap282:6.4": "cakkavattī mahabbalo.",
"tha-ap282:7.1": "Paṭisambhidā catasso, … pe …",
"tha-ap282:7.4": "kataṃ buddhassa sāsanaṃ”. #2618",
"tha-ap282:8.1": "Itthaṃ sudaṃ āyasmā taraṇīyo thero imā gāthāyo abhāsitthāti.",
"tha-ap282:9.1": "Taraṇīyattherassāpadānaṃ dasamaṃ.",
"tha-ap282:10.1": "Suvaṇṇabibbohanavaggo aṭṭhavīsatimo.",
"tha-ap282:11.1": "Tassuddānaṃ",
"tha-ap282:12.1": "Suvaṇṇaṃ tilamuṭṭhi ca,",
"tha-ap282:12.2": "caṅkoṭabbhañjanañjalī;",
"tha-ap282:12.3": "Potthako citamāluvā,",
"tha-ap282:12.4": "ekapuṇḍarī setunā;",
"tha-ap282:12.5": "Dvecattālīsa gāthāyo,",
"tha-ap282:12.6": "gaṇitāyo vibhāvibhīti.",
"tha-ap282:13.1": "Ekādasamaṃ bhāṇavāraṃ."
}