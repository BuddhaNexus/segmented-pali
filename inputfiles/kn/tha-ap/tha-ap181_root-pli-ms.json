{
"tha-ap181:1.1": "Therāpadāna",
"tha-ap181:2.1": "Kumudavagga",
"tha-ap181:3.1": "9. Tikicchakattheraapadāna",
"tha-ap181:4.1": "“Nagare bandhumatiyā,",
"tha-ap181:4.2": "vejjo āsiṃ susikkhito;",
"tha-ap181:4.3": "Āturānaṃ sadukkhānaṃ,",
"tha-ap181:4.4": "mahājanasukhāvaho.",
"tha-ap181:5.1": "Byādhitaṃ samaṇaṃ disvā,",
"tha-ap181:5.2": "sīlavantaṃ mahājutiṃ;",
"tha-ap181:5.3": "Pasannacitto sumano,",
"tha-ap181:5.4": "bhesajjamadadiṃ tadā.",
"tha-ap181:6.1": "Arogo āsi teneva,",
"tha-ap181:6.2": "samaṇo saṃvutindriyo;",
"tha-ap181:6.3": "Asoko nāma nāmena,",
"tha-ap181:6.4": "upaṭṭhāko vipassino.",
"tha-ap181:7.1": "Ekanavutito kappe,",
"tha-ap181:7.2": "yaṃ osadhamadāsahaṃ;",
"tha-ap181:7.3": "Duggatiṃ nābhijānāmi,",
"tha-ap181:7.4": "bhesajjassa idaṃ phalaṃ.",
"tha-ap181:8.1": "Ito ca aṭṭhame kappe,",
"tha-ap181:8.2": "sabbosadhasanāmako;",
"tha-ap181:8.3": "Sattaratanasampanno,",
"tha-ap181:8.4": "cakkavattī mahabbalo.",
"tha-ap181:9.1": "Paṭisambhidā catasso, … pe …",
"tha-ap181:9.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap181:10.1": "Itthaṃ sudaṃ āyasmā tikicchako thero imā gāthāyo abhāsitthāti.",
"tha-ap181:11.1": "Tikicchakattherassāpadānaṃ navamaṃ."
}