{
"tha-ap123:1.1": "Therāpadāna",
"tha-ap123:2.1": "Sereyyavagga",
"tha-ap123:3.1": "1. Sereyyakattheraapadāna",
"tha-ap123:4.1": "“Ajjhāyako mantadharo,",
"tha-ap123:4.2": "tiṇṇaṃ vedāna pāragū;",
"tha-ap123:4.3": "Abbhokāse ṭhito santo,",
"tha-ap123:4.4": "addasaṃ lokanāyakaṃ.",
"tha-ap123:5.1": "Sīhaṃ yathā vanacaraṃ,",
"tha-ap123:5.2": "byaggharājaṃva nittasaṃ;",
"tha-ap123:5.3": "Tidhāpabhinnamātaṅgaṃ,",
"tha-ap123:5.4": "kuñjaraṃva mahesinaṃ.",
"tha-ap123:6.1": "Sereyyakaṃ gahetvāna,",
"tha-ap123:6.2": "ākāse ukkhipiṃ ahaṃ;",
"tha-ap123:6.3": "Buddhassa ānubhāvena,",
"tha-ap123:6.4": "parivārenti sabbaso.",
"tha-ap123:7.1": "Adhiṭṭhahi mahāvīro,",
"tha-ap123:7.2": "sabbaññū lokanāyako;",
"tha-ap123:7.3": "Samantā pupphacchadanā,",
"tha-ap123:7.4": "okiriṃsu narāsabhaṃ.",
"tha-ap123:8.1": "Tato sā pupphakañcukā,",
"tha-ap123:8.2": "antovaṇṭā bahimukhā;",
"tha-ap123:8.3": "Sattāhaṃ chadanaṃ katvā,",
"tha-ap123:8.4": "tato antaradhāyatha.",
"tha-ap123:9.1": "Tañca acchariyaṃ disvā,",
"tha-ap123:9.2": "abbhutaṃ lomahaṃsanaṃ;",
"tha-ap123:9.3": "Buddhe cittaṃ pasādesiṃ,",
"tha-ap123:9.4": "sugate lokanāyake.",
"tha-ap123:10.1": "Tena cittappasādena,",
"tha-ap123:10.2": "sukkamūlena codito;",
"tha-ap123:10.3": "Kappānaṃ satasahassaṃ,",
"tha-ap123:10.4": "duggatiṃ nupapajjahaṃ.",
"tha-ap123:11.1": "Pannarasasahassamhi,",
"tha-ap123:11.2": "kappānaṃ pañcavīsati;",
"tha-ap123:11.3": "Vītamalā samānā ca,",
"tha-ap123:11.4": "cakkavattī mahabbalā.",
"tha-ap123:12.1": "Paṭisambhidā catasso,",
"tha-ap123:12.2": "vimokkhāpi ca aṭṭhime;",
"tha-ap123:12.3": "Chaḷabhiññā sacchikatā,",
"tha-ap123:12.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap123:13.1": "Itthaṃ sudaṃ āyasmā sereyyako thero imā gāthāyo abhāsitthāti.",
"tha-ap123:14.1": "Sereyyakattherassāpadānaṃ paṭhamaṃ."
}