{
"tha-ap166:1.1": "Therāpadāna",
"tha-ap166:2.1": "Supāricariyavagga",
"tha-ap166:3.1": "4. Desapūjakattheraapadāna",
"tha-ap166:4.1": "“Atthadassī tu bhagavā,",
"tha-ap166:4.2": "lokajeṭṭho narāsabho;",
"tha-ap166:4.3": "Abbhuggantvāna vehāsaṃ,",
"tha-ap166:4.4": "gacchate anilañjase.",
"tha-ap166:5.1": "Yamhi dese ṭhito satthā,",
"tha-ap166:5.2": "abbhuggacchi mahāmuni;",
"tha-ap166:5.3": "Tāhaṃ desaṃ apūjesiṃ,",
"tha-ap166:5.4": "pasanno sehi pāṇibhi.",
"tha-ap166:6.1": "Aṭṭhārase kappasate,",
"tha-ap166:6.2": "addasaṃ yaṃ mahāmuniṃ;",
"tha-ap166:6.3": "Duggatiṃ nābhijānāmi,",
"tha-ap166:6.4": "desapūjāyidaṃ phalaṃ.",
"tha-ap166:7.1": "Ekādase kappasate,",
"tha-ap166:7.2": "gosujātasanāmako;",
"tha-ap166:7.3": "Sattaratanasampanno,",
"tha-ap166:7.4": "cakkavattī mahabbalo.",
"tha-ap166:8.1": "Paṭisambhidā catasso, … pe …",
"tha-ap166:8.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap166:9.1": "Itthaṃ sudaṃ āyasmā desapūjako thero imā gāthāyo abhāsitthāti.",
"tha-ap166:10.1": "Desapūjakattherassāpadānaṃ catutthaṃ."
}