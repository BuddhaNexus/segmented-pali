{
"tha-ap141:1.1": "Therāpadāna",
"tha-ap141:2.1": "Sobhitavagga",
"tha-ap141:3.1": "9 Sālapupphadāyakattheraapadāna",
"tha-ap141:4.1": "“Migarājā tadā āsiṃ,",
"tha-ap141:4.2": "abhijāto sukesarī;",
"tha-ap141:4.3": "Giriduggaṃ gavesanto,",
"tha-ap141:4.4": "addasaṃ lokanāyakaṃ.",
"tha-ap141:5.1": "Ayaṃ nu kho mahāvīro,",
"tha-ap141:5.2": "nibbāpeti mahājanaṃ;",
"tha-ap141:5.3": "Yannūnāhaṃ upāseyyaṃ,",
"tha-ap141:5.4": "devadevaṃ narāsabhaṃ.",
"tha-ap141:6.1": "Sākhaṃ sālassa bhañjitvā,",
"tha-ap141:6.2": "sakosaṃ pupphamāhariṃ;",
"tha-ap141:6.3": "Upagantvāna sambuddhaṃ,",
"tha-ap141:6.4": "adāsiṃ pupphamuttamaṃ.",
"tha-ap141:7.1": "Ekanavutito kappe,",
"tha-ap141:7.2": "yaṃ pupphamabhiropayiṃ;",
"tha-ap141:7.3": "Duggatiṃ nābhijānāmi,",
"tha-ap141:7.4": "pupphadānassidaṃ phalaṃ.",
"tha-ap141:8.1": "Ito ca navame kappe,",
"tha-ap141:8.2": "virocanasanāmakā;",
"tha-ap141:8.3": "Tayo āsiṃsu rājāno,",
"tha-ap141:8.4": "cakkavattī mahabbalā.",
"tha-ap141:9.1": "Paṭisambhidā catasso, … pe …",
"tha-ap141:9.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap141:10.1": "Itthaṃ sudaṃ āyasmā sālapupphadāyako thero imā gāthāyo abhāsitthāti.",
"tha-ap141:11.1": "Sālapupphadāyakattherassāpadānaṃ navamaṃ."
}