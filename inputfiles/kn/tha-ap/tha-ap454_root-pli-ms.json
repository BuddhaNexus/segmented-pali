{
"tha-ap454:1.1": "Therāpadāna",
"tha-ap454:2.1": "Jagatidāyakavagga",
"tha-ap454:3.1": "2. Morahatthiyattheraapadāna",
"tha-ap454:4.1": "“Morahatthaṃ gahetvāna,",
"tha-ap454:4.2": "upesiṃ lokanāyakaṃ;",
"tha-ap454:4.3": "Pasannacitto sumano,",
"tha-ap454:4.4": "morahatthamadāsahaṃ.",
"tha-ap454:5.1": "Iminā morahatthena,",
"tha-ap454:5.2": "cetanāpaṇidhīhi ca;",
"tha-ap454:5.3": "Nibbāyiṃsu tayo aggī,",
"tha-ap454:5.4": "labhāmi vipulaṃ sukhaṃ.",
"tha-ap454:6.1": "Aho buddhā aho dhammā,",
"tha-ap454:6.2": "aho no satthusampadā;",
"tha-ap454:6.3": "Datvānahaṃ morahatthaṃ,",
"tha-ap454:6.4": "labhāmi vipulaṃ sukhaṃ.",
"tha-ap454:7.1": "Tiyaggī nibbutā mayhaṃ,",
"tha-ap454:7.2": "bhavā sabbe samūhatā;",
"tha-ap454:7.3": "Sabbāsavā parikkhīṇā,",
"tha-ap454:7.4": "natthi dāni punabbhavo.",
"tha-ap454:8.1": "Ekatiṃse ito kappe,",
"tha-ap454:8.2": "yaṃ dānamadadiṃ tadā;",
"tha-ap454:8.3": "Duggatiṃ nābhijānāmi,",
"tha-ap454:8.4": "morahatthassidaṃ phalaṃ.",
"tha-ap454:9.1": "Kilesā jhāpitā mayhaṃ, … pe …",
"tha-ap454:9.4": "viharāmi anāsavo.",
"tha-ap454:10.1": "Svāgataṃ vata me āsi, … pe …",
"tha-ap454:10.4": "kataṃ buddhassa sāsanaṃ.",
"tha-ap454:11.1": "Paṭisambhidā catasso, … pe …",
"tha-ap454:11.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap454:12.1": "Itthaṃ sudaṃ āyasmā morahatthiyo thero imā gāthāyo abhāsitthāti.",
"tha-ap454:13.1": "Morahatthiyattherassāpadānaṃ dutiyaṃ."
}