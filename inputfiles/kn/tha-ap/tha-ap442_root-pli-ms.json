{
"tha-ap442:1.1": "Therāpadāna",
"tha-ap442:2.1": "Ekavihārivagga",
"tha-ap442:3.1": "10 Ambaphaliyattheraapadāna",
"tha-ap442:4.1": "“Padumuttarabuddhassa,",
"tha-ap442:4.2": "lokajeṭṭhassa tādino;",
"tha-ap442:4.3": "Piṇḍāya vicarantassa,",
"tha-ap442:4.4": "dhārato uttamaṃ yasaṃ.",
"tha-ap442:5.1": "Aggaphalaṃ gahetvāna,",
"tha-ap442:5.2": "vippasannena cetasā;",
"tha-ap442:5.3": "Dakkhiṇeyyassa vīrassa,",
"tha-ap442:5.4": "adāsiṃ satthuno ahaṃ.",
"tha-ap442:6.1": "Tena kammena dvipadinda,",
"tha-ap442:6.2": "lokajeṭṭha narāsabha;",
"tha-ap442:6.3": "Pattomhi acalaṃ ṭhānaṃ,",
"tha-ap442:6.4": "hitvā jayaparājayaṃ.",
"tha-ap442:7.1": "Satasahassito kappe,",
"tha-ap442:7.2": "yaṃ dānamadadiṃ tadā;",
"tha-ap442:7.3": "Duggatiṃ nābhijānāmi,",
"tha-ap442:7.4": "aggadānassidaṃ phalaṃ.",
"tha-ap442:8.1": "Kilesā jhāpitā mayhaṃ, … pe …",
"tha-ap442:8.4": "viharāmi anāsavo.",
"tha-ap442:9.1": "Svāgataṃ vata me āsi, … pe …",
"tha-ap442:9.4": "kataṃ buddhassa sāsanaṃ.",
"tha-ap442:10.1": "Paṭisambhidā catasso, … pe …",
"tha-ap442:10.4": "kataṃ buddhassa sāsanaṃ”. #4624",
"tha-ap442:11.1": "Itthaṃ sudaṃ āyasmā ambaphaliyo thero imā gāthāyo abhāsitthāti.",
"tha-ap442:12.1": "Ambaphaliyattherassāpadānaṃ dasamaṃ.",
"tha-ap442:13.1": "Ekavihārivaggo catucattālīsamo.",
"tha-ap442:14.1": "Tassuddānaṃ",
"tha-ap442:15.1": "Thero ekavihārī ca,",
"tha-ap442:15.2": "saṅkhiyo pāṭihīrako;",
"tha-ap442:15.3": "Thaviko ucchukhaṇḍī ca,",
"tha-ap442:15.4": "kaḷambaambāṭakado.",
"tha-ap442:16.1": "Harītakambapiṇḍī ca,",
"tha-ap442:16.2": "ambado dasamo yati;",
"tha-ap442:16.3": "Chaḷasīti ca gāthāyo,",
"tha-ap442:16.4": "gaṇitāyo vibhāvibhi."
}