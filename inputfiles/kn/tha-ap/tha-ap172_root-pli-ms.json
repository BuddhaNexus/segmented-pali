{
"tha-ap172:1.1": "Therāpadāna",
"tha-ap172:2.1": "Supāricariyavagga",
"tha-ap172:3.1": "10. Pañcaṅguliyattheraapadāna",
"tha-ap172:4.1": "“Tisso nāmāsi bhagavā,",
"tha-ap172:4.2": "lokajeṭṭho narāsabho;",
"tha-ap172:4.3": "Pavisati gandhakuṭiṃ,",
"tha-ap172:4.4": "vihārakusalo muni.",
"tha-ap172:5.1": "Sugandhamālamādāya,",
"tha-ap172:5.2": "agamāsiṃ jinantikaṃ;",
"tha-ap172:5.3": "Apasaddo ca sambuddhe,",
"tha-ap172:5.4": "pañcaṅgulimadāsahaṃ.",
"tha-ap172:6.1": "Dvenavute ito kappe,",
"tha-ap172:6.2": "yaṃ gandhamabhiropayiṃ;",
"tha-ap172:6.3": "Duggatiṃ nābhijānāmi,",
"tha-ap172:6.4": "pañcaṅgulissidaṃ phalaṃ.",
"tha-ap172:7.1": "Dvesattatimhito kappe,",
"tha-ap172:7.2": "rājā āsiṃ sayampabho;",
"tha-ap172:7.3": "Sattaratanasampanno,",
"tha-ap172:7.4": "cakkavattī mahabbalo.",
"tha-ap172:8.1": "Paṭisambhidā catasso, … pe …",
"tha-ap172:8.4": "kataṃ buddhassa sāsanaṃ”. #2098",
"tha-ap172:9.1": "Itthaṃ sudaṃ āyasmā pañcaṅguliyo thero imā gāthāyo abhāsitthāti.",
"tha-ap172:10.1": "Pañcaṅguliyattherassāpadānaṃ dasamaṃ.",
"tha-ap172:11.1": "Supāricariyavaggo sattarasamo.",
"tha-ap172:12.1": "Tassuddānaṃ",
"tha-ap172:13.1": "Supāricari kaṇaverī,",
"tha-ap172:13.2": "khajjako desapūjako;",
"tha-ap172:13.3": "Kaṇikāro sappidado,",
"tha-ap172:13.4": "yūthiko dussadāyako;",
"tha-ap172:13.5": "Māḷo ca pañcaṅguliko,",
"tha-ap172:13.6": "catupaññāsa gāthakāti."
}