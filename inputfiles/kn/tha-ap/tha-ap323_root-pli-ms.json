{
  "tha-ap323:0.1": "Therāpadāna ",
  "tha-ap323:0.2": "Umāpupphiyavagga ",
  "tha-ap323:0.3": "1 Umāpupphiyattheraapadāna ",
  "tha-ap323:1.1": "“Samāhitaṁ samāpannaṁ, ",
  "tha-ap323:1.2": "siddhatthamaparājitaṁ; ",
  "tha-ap323:1.3": "Samādhinā upaviṭṭhaṁ, ",
  "tha-ap323:1.4": "addasāhaṁ naruttamaṁ. ",
  "tha-ap323:2.1": "Umāpupphaṁ gahetvāna, ",
  "tha-ap323:2.2": "buddhassa abhiropayiṁ; ",
  "tha-ap323:2.3": "Sabbapupphā ekasīsā, ",
  "tha-ap323:2.4": "uddhaṁvaṇṭā adhomukhā. ",
  "tha-ap323:3.1": "Sucittā viya tiṭṭhante, ",
  "tha-ap323:3.2": "ākāse pupphasantharā; ",
  "tha-ap323:3.3": "Tena cittappasādena, ",
  "tha-ap323:3.4": "tusitaṁ upapajjahaṁ. ",
  "tha-ap323:4.1": "Catunnavutito kappe, ",
  "tha-ap323:4.2": "yaṁ pupphamabhipūjayiṁ; ",
  "tha-ap323:4.3": "Duggatiṁ nābhijānāmi, ",
  "tha-ap323:4.4": "buddhapūjāyidaṁ phalaṁ. ",
  "tha-ap323:5.1": "Pañcapaññāsito kappe, ",
  "tha-ap323:5.2": "eko āsiṁ mahīpati; ",
  "tha-ap323:5.3": "Samantachadano nāma, ",
  "tha-ap323:5.4": "cakkavattī mahabbalo. ",
  "tha-ap323:6.1": "Paṭisambhidā catasso, ",
  "tha-ap323:6.2": "vimokkhāpi ca aṭṭhime; ",
  "tha-ap323:6.3": "Chaḷabhiññā sacchikatā, ",
  "tha-ap323:6.4": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap323:7.1": "Itthaṁ sudaṁ āyasmā umāpupphiyo thero imā gāthāyo abhāsitthāti. ",
  "tha-ap323:8.1": "Umāpupphiyattherassāpadānaṁ paṭhamaṁ. "
}