{
"tha-ap205:1.1": "Therāpadāna",
"tha-ap205:2.1": "Kaṇikārapupphiyavagga",
"tha-ap205:3.1": "3 Kiṅkaṇipupphiyattheraapadāna",
"tha-ap205:4.1": "“Kañcanagghiyasaṅkāso,",
"tha-ap205:4.2": "sabbaññū lokanāyako;",
"tha-ap205:4.3": "Odakaṃ dahamoggayha,",
"tha-ap205:4.4": "sināyi lokanāyako.",
"tha-ap205:5.1": "Paggayha kiṅkaṇiṃ pupphaṃ,",
"tha-ap205:5.2": "Vipassissābhiropayiṃ;",
"tha-ap205:5.3": "Udaggacitto sumano,",
"tha-ap205:5.4": "Dvipadindassa tādino.",
"tha-ap205:6.1": "Ekanavutito kappe,",
"tha-ap205:6.2": "yaṃ pupphamabhiropayiṃ;",
"tha-ap205:6.3": "Duggatiṃ nābhijānāmi,",
"tha-ap205:6.4": "buddhapūjāyidaṃ phalaṃ.",
"tha-ap205:7.1": "Sattavīsatikappamhi,",
"tha-ap205:7.2": "rājā bhīmaratho ahu;",
"tha-ap205:7.3": "Sattaratanasampanno,",
"tha-ap205:7.4": "cakkavattī mahabbalo.",
"tha-ap205:8.1": "Paṭisambhidā catasso, … pe …",
"tha-ap205:8.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap205:9.1": "Itthaṃ sudaṃ āyasmā kiṅkaṇipupphiyo thero imā gāthāyo abhāsitthāti.",
"tha-ap205:10.1": "Kiṅkaṇipupphiyattherassāpadānaṃ tatiyaṃ."
}