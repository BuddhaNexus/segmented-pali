{
"tha-ap1:1.1": "Therāpadāna",
"tha-ap1:2.1": "Buddhavagga",
"tha-ap1:3.1": "1. Buddhaapadāna",
"tha-ap1:4.1": "Namo tassa Bhagavato Arahato Sammāsambuddhassa.",
"tha-ap1:5.1": "Tathāgataṃ jetavane vasantaṃ,",
"tha-ap1:5.2": "Apucchi vedehamunī nataṅgo;",
"tha-ap1:5.3": "“Sabbaññubuddhā kira nāma honti,",
"tha-ap1:5.4": "Bhavanti te hetubhi kehi vīra”.",
"tha-ap1:6.1": "Tadāha sabbaññuvaro mahesī,",
"tha-ap1:6.2": "Ānandabhaddaṃ madhurassarena;",
"tha-ap1:6.3": "“Ye pubbabuddhesu katādhikārā,",
"tha-ap1:6.4": "Aladdhamokkhā jinasāsanesu.",
"tha-ap1:7.1": "Teneva sambodhimukhena dhīrā,",
"tha-ap1:7.2": "Ajjhāsayenāpi mahābalena;",
"tha-ap1:7.3": "Paññāya tejena sutikkhapaññā,",
"tha-ap1:7.4": "Sabbaññubhāvaṃ anupāpuṇanti.",
"tha-ap1:8.1": "Ahampi pubbabuddhesu,",
"tha-ap1:8.2": "buddhattamabhipatthayiṃ;",
"tha-ap1:8.3": "Manasāyeva hutvāna,",
"tha-ap1:8.4": "dhammarājā asaṅkhiyā.",
"tha-ap1:9.1": "Atha buddhāpadānāni,",
"tha-ap1:9.2": "suṇātha suddhamānasā;",
"tha-ap1:9.3": "Tiṃsapāramisampuṇṇā,",
"tha-ap1:9.4": "dhammarājā asaṅkhiyā.",
"tha-ap1:10.1": "Sambodhiṃ buddhaseṭṭhānaṃ,",
"tha-ap1:10.2": "sasaṃghe lokanāyake;",
"tha-ap1:10.3": "Dasaṅgulī namassitvā,",
"tha-ap1:10.4": "sirasā abhivādayiṃ.",
"tha-ap1:11.1": "Yāvatā buddhakhettesu,",
"tha-ap1:11.2": "ratanā vijjantisaṅkhiyā;",
"tha-ap1:11.3": "Ākāsaṭṭhā ca bhūmaṭṭhā,",
"tha-ap1:11.4": "manasā sabbamāhariṃ.",
"tha-ap1:12.1": "Tattha rūpiyabhūmiyaṃ,",
"tha-ap1:12.2": "pāsādaṃ māpayiṃ ahaṃ;",
"tha-ap1:12.3": "Nekabhummaṃ ratanāmayaṃ,",
"tha-ap1:12.4": "ubbiddhaṃ nabhamuggataṃ.",
"tha-ap1:13.1": "Vicittathambhaṃ sukataṃ,",
"tha-ap1:13.2": "suvibhattaṃ mahārahaṃ;",
"tha-ap1:13.3": "Kanakamayasaṅghāṭaṃ,",
"tha-ap1:13.4": "kontacchattehi maṇḍitaṃ.",
"tha-ap1:14.1": "Paṭhamā veḷuriyā bhūmi,",
"tha-ap1:14.2": "vimalabbhasamā subhā;",
"tha-ap1:14.3": "Naḷinajalajākiṇṇā,",
"tha-ap1:14.4": "varakañcanabhūmiyā.",
"tha-ap1:15.1": "Pavāḷaṃsā pavāḷavaṇṇā,",
"tha-ap1:15.2": "kāci lohitakā subhā;",
"tha-ap1:15.3": "Indagopakavaṇṇābhā,",
"tha-ap1:15.4": "bhūmi obhāsatī disā.",
"tha-ap1:16.1": "Suvibhattā gharamukhā,",
"tha-ap1:16.2": "niyyūhā sīhapañjarā;",
"tha-ap1:16.3": "Caturo vedikā jālā,",
"tha-ap1:16.4": "gandhāveḷā manoramā.",
"tha-ap1:17.1": "Nīlā pītā lohitakā,",
"tha-ap1:17.2": "odātā suddhakāḷakā;",
"tha-ap1:17.3": "Kūṭāgāravarūpetā,",
"tha-ap1:17.4": "sattaratanabhūsitā.",
"tha-ap1:18.1": "Olokamayā padumā,",
"tha-ap1:18.2": "vāḷavihaṅgasobhitā;",
"tha-ap1:18.3": "Nakkhattatārakākiṇṇā,",
"tha-ap1:18.4": "candasūrehi maṇḍitā.",
"tha-ap1:19.1": "Hemajālena sañchannā,",
"tha-ap1:19.2": "soṇṇakiṅkiṇikāyutā;",
"tha-ap1:19.3": "Vātavegena kūjanti,",
"tha-ap1:19.4": "soṇṇamālā manoramā.",
"tha-ap1:20.1": "Mañjeṭṭhakaṃ lohitakaṃ,",
"tha-ap1:20.2": "pītakaṃ haripiñjaraṃ;",
"tha-ap1:20.3": "Nānāraṅgehi sampītaṃ,",
"tha-ap1:20.4": "ussitaddhajamālinī.",
"tha-ap1:21.1": "Na naṃ bahūnekasatā,",
"tha-ap1:21.2": "phalikā rajatāmayā;",
"tha-ap1:21.3": "Maṇimayā lohitaṅgā,",
"tha-ap1:21.4": "masāragallamayā tathā;",
"tha-ap1:21.5": "Nānāsayanavicittā,",
"tha-ap1:21.6": "saṇhakāsikasanthatā.",
"tha-ap1:22.1": "Kampalā dukūlā cīnā,",
"tha-ap1:22.2": "pattuṇṇā paṇḍupāvurā;",
"tha-ap1:22.3": "Vividhattharaṇaṃ sabbaṃ,",
"tha-ap1:22.4": "manasā paññapesahaṃ.",
"tha-ap1:23.1": "Tāsu tāsveva bhūmīsu,",
"tha-ap1:23.2": "ratanakūṭalaṅkataṃ;",
"tha-ap1:23.3": "Maṇiverocanā ukkā,",
"tha-ap1:23.4": "dhārayantā sutiṭṭhare.",
"tha-ap1:24.1": "Sobhanti esikā thambhā,",
"tha-ap1:24.2": "subhā kañcanatoraṇā;",
"tha-ap1:24.3": "Jambonadā sāramayā,",
"tha-ap1:24.4": "atho rajatamayāpi ca.",
"tha-ap1:25.1": "Nekā sandhī suvibhattā,",
"tha-ap1:25.2": "kavāṭaggaḷacittitā;",
"tha-ap1:25.3": "Ubhato puṇṇaghaṭānekā,",
"tha-ap1:25.4": "padumuppalasaṃyutā.",
"tha-ap1:26.1": "Atīte sabbabuddhe ca,",
"tha-ap1:26.2": "sasaṃghe lokanāyake;",
"tha-ap1:26.3": "Pakativaṇṇarūpena,",
"tha-ap1:26.4": "nimminitvā sasāvake.",
"tha-ap1:27.1": "Tena dvārena pavisitvā,",
"tha-ap1:27.2": "sabbe buddhā sasāvakā;",
"tha-ap1:27.3": "Sabbasoṇṇamaye pīṭhe,",
"tha-ap1:27.4": "nisinnā ariyamaṇḍalā.",
"tha-ap1:28.1": "Ye ca etarahi atthi,",
"tha-ap1:28.2": "Buddhā loke anuttarā;",
"tha-ap1:28.3": "Atīte vattamānā ca,",
"tha-ap1:28.4": "Bhavanaṃ sabbe samāruhuṃ.",
"tha-ap1:29.1": "Paccekabuddhenekasate,",
"tha-ap1:29.2": "Sayambhū aparājite;",
"tha-ap1:29.3": "Atīte vattamāne ca,",
"tha-ap1:29.4": "Bhavanaṃ sabbe samāruhuṃ.",
"tha-ap1:30.1": "Kapparukkhā bahū atthi,",
"tha-ap1:30.2": "Ye dibbā ye ca mānusā;",
"tha-ap1:30.3": "Sabbaṃ dussaṃ samāhantvā,",
"tha-ap1:30.4": "Acchādemi ticīvaraṃ.",
"tha-ap1:31.1": "Khajjaṃ bhojjaṃ sāyanīyaṃ,",
"tha-ap1:31.2": "sampannaṃ pānabhojanaṃ;",
"tha-ap1:31.3": "Maṇimaye subhe patte,",
"tha-ap1:31.4": "sampūretvā adāsahaṃ.",
"tha-ap1:32.1": "Dibbavatthasamā hutvā,",
"tha-ap1:32.2": "maṭṭhā cīvarasaṃyutā;",
"tha-ap1:32.3": "Madhurā sakkharā ceva,",
"tha-ap1:32.4": "telā ca madhuphāṇitā.",
"tha-ap1:33.1": "Tappitā paramannena,",
"tha-ap1:33.2": "sabbe te ariyamaṇḍalā;",
"tha-ap1:33.3": "Ratanagabbhaṃ pavisitvā,",
"tha-ap1:33.4": "kesarīva guhāsayā.",
"tha-ap1:34.1": "Mahārahamhi sayane,",
"tha-ap1:34.2": "sīhaseyyamakappayuṃ;",
"tha-ap1:34.3": "Sampajānā samuṭṭhāya,",
"tha-ap1:34.4": "sayane pallaṅkamābhujuṃ.",
"tha-ap1:35.1": "Gocaraṃ sabbabuddhānaṃ,",
"tha-ap1:35.2": "jhānaratisamappitā;",
"tha-ap1:35.3": "Aññe dhammāni desenti,",
"tha-ap1:35.4": "aññe kīḷanti iddhiyā.",
"tha-ap1:36.1": "Aññe abhiññā appenti,",
"tha-ap1:36.2": "abhiññā vasibhāvitā;",
"tha-ap1:36.3": "Vikubbanā vikubbanti,",
"tha-ap1:36.4": "aññenekasahassiyo.",
"tha-ap1:37.1": "Buddhāpi buddhe pucchanti,",
"tha-ap1:37.2": "visayaṃ sabbaññumālayaṃ;",
"tha-ap1:37.3": "Gambhīraṃ nipuṇaṃ ṭhānaṃ,",
"tha-ap1:37.4": "paññāya vinibujjhare.",
"tha-ap1:38.1": "Sāvakā buddhe pucchanti,",
"tha-ap1:38.2": "buddhā pucchanti sāvake;",
"tha-ap1:38.3": "Aññamaññañca pucchitvā,",
"tha-ap1:38.4": "aññoññaṃ byākaronti te.",
"tha-ap1:39.1": "Buddhā paccekabuddhā ca,",
"tha-ap1:39.2": "sāvakā paricārakā;",
"tha-ap1:39.3": "Evaṃ sakāya ratiyā,",
"tha-ap1:39.4": "pāsādebhiramanti te.",
"tha-ap1:40.1": "Chattā tiṭṭhantu ratanā,",
"tha-ap1:40.2": "kañcanāveḷapantikā;",
"tha-ap1:40.3": "Muttājālaparikkhittā,",
"tha-ap1:40.4": "sabbe dhārentu matthake.",
"tha-ap1:41.1": "Bhavantu ceḷavitānā,",
"tha-ap1:41.2": "soṇṇatārakacittitā;",
"tha-ap1:41.3": "Vicittamalyavitatā,",
"tha-ap1:41.4": "sabbe dhārentu matthake.",
"tha-ap1:42.1": "Vitatā malyadāmehi,",
"tha-ap1:42.2": "gandhadāmehi sobhitā;",
"tha-ap1:42.3": "Dussadāmaparikiṇṇā,",
"tha-ap1:42.4": "ratanadāmabhūsitā.",
"tha-ap1:43.1": "Pupphābhikiṇṇā sucittā,",
"tha-ap1:43.2": "surabhigandhabhūsitā;",
"tha-ap1:43.3": "Gandhapañcaṅgulakatā,",
"tha-ap1:43.4": "hemacchadanachāditā.",
"tha-ap1:44.1": "Catuddisā pokkharañño,",
"tha-ap1:44.2": "padumuppalasanthatā;",
"tha-ap1:44.3": "Sovaṇṇarūpā khāyantu,",
"tha-ap1:44.4": "padmareṇurajuggatā.",
"tha-ap1:45.1": "Pupphantu pādapā sabbe,",
"tha-ap1:45.2": "pāsādassa samantato;",
"tha-ap1:45.3": "Sayañca pupphā muñcitvā,",
"tha-ap1:45.4": "gantvā bhavanamokiruṃ.",
"tha-ap1:46.1": "Sikhino tattha naccantu,",
"tha-ap1:46.2": "dibbahaṃsā pakūjare;",
"tha-ap1:46.3": "Karavīkā ca gāyantu,",
"tha-ap1:46.4": "dijasaṅghā samantato.",
"tha-ap1:47.1": "Bheriyo sabbā vajjantu,",
"tha-ap1:47.2": "vīṇā sabbā rasantu tā;",
"tha-ap1:47.3": "Sabbā saṅgīti vattantu,",
"tha-ap1:47.4": "pāsādassa samantato.",
"tha-ap1:48.1": "Yāvatā buddhakhettamhi,",
"tha-ap1:48.2": "cakkavāḷe tato pare;",
"tha-ap1:48.3": "Mahantā jotisampannā,",
"tha-ap1:48.4": "acchinnā ratanāmayā.",
"tha-ap1:49.1": "Tiṭṭhantu soṇṇapallaṅkā,",
"tha-ap1:49.2": "dīparukkhā jalantu te;",
"tha-ap1:49.3": "Bhavantu ekapajjotā,",
"tha-ap1:49.4": "dasasahassiparamparā.",
"tha-ap1:50.1": "Gaṇikā lāsikā ceva,",
"tha-ap1:50.2": "naccantu accharāgaṇā;",
"tha-ap1:50.3": "Nānāraṅgā padissantu,",
"tha-ap1:50.4": "pāsādassa samantato.",
"tha-ap1:51.1": "Dumagge pabbatagge vā,",
"tha-ap1:51.2": "sinerugirimuddhani;",
"tha-ap1:51.3": "Ussāpemi dhajaṃ sabbaṃ,",
"tha-ap1:51.4": "vicittaṃ pañcavaṇṇikaṃ.",
"tha-ap1:52.1": "Narā nāgā ca gandhabbā,",
"tha-ap1:52.2": "sabbe devā upentu te;",
"tha-ap1:52.3": "Namassantā pañjalikā,",
"tha-ap1:52.4": "pāsādaṃ parivārayuṃ.",
"tha-ap1:53.1": "Yaṃ kiñci kusalaṃ kammaṃ,",
"tha-ap1:53.2": "kattabbaṃ kiriyaṃ mama;",
"tha-ap1:53.3": "Kāyena vācā manasā,",
"tha-ap1:53.4": "tidase sukataṃ kataṃ.",
"tha-ap1:54.1": "Ye sattā saññino atthi,",
"tha-ap1:54.2": "ye ca sattā asaññino;",
"tha-ap1:54.3": "Kataṃ puññaphalaṃ mayhaṃ,",
"tha-ap1:54.4": "sabbe bhāgī bhavantu te.",
"tha-ap1:55.1": "Yesaṃ kataṃ suviditaṃ,",
"tha-ap1:55.2": "dinnaṃ puññaphalaṃ mayā;",
"tha-ap1:55.3": "Ye ca tattha na jānanti,",
"tha-ap1:55.4": "devā gantvā nivedayuṃ.",
"tha-ap1:56.1": "Sabbalokamhi ye sattā,",
"tha-ap1:56.2": "Jīvantāhārahetukā;",
"tha-ap1:56.3": "Manuññaṃ bhojanaṃ sabbaṃ,",
"tha-ap1:56.4": "Labhantu mama cetasā.",
"tha-ap1:57.1": "Manasā dānaṃ mayā dinnaṃ,",
"tha-ap1:57.2": "manasā pasādamāvahiṃ;",
"tha-ap1:57.3": "Pūjitā sabbasambuddhā,",
"tha-ap1:57.4": "paccekā jinasāvakā.",
"tha-ap1:58.1": "Tena kammena sukatena,",
"tha-ap1:58.2": "cetanāpaṇidhīhi ca;",
"tha-ap1:58.3": "Jahitvā mānusaṃ dehaṃ,",
"tha-ap1:58.4": "tāvatiṃsamagacchahaṃ.",
"tha-ap1:59.1": "Duve bhave pajānāmi,",
"tha-ap1:59.2": "devatte atha mānuse;",
"tha-ap1:59.3": "Aññaṃ gatiṃ na jānāmi,",
"tha-ap1:59.4": "manasā patthanāphalaṃ.",
"tha-ap1:60.1": "Devānaṃ adhiko homi,",
"tha-ap1:60.2": "bhavāmi manujādhipo;",
"tha-ap1:60.3": "Rūpalakkhaṇasampanno,",
"tha-ap1:60.4": "paññāya asamo bhave.",
"tha-ap1:61.1": "Bhojanaṃ vividhaṃ seṭṭhaṃ,",
"tha-ap1:61.2": "Ratanañca anappakaṃ;",
"tha-ap1:61.3": "Vividhāni ca vatthāni,",
"tha-ap1:61.4": "Nabhā khippaṃ upenti maṃ.",
"tha-ap1:62.1": "Pathabyā pabbate ceva,",
"tha-ap1:62.2": "ākāse udake vane;",
"tha-ap1:62.3": "Yaṃ yaṃ hatthaṃ pasāremi,",
"tha-ap1:62.4": "dibbā bhakkhā upenti maṃ.",
"tha-ap1:63.1": "Pathabyā pabbate ceva,",
"tha-ap1:63.2": "ākāse udake vane;",
"tha-ap1:63.3": "Yaṃ yaṃ hatthaṃ pasāremi,",
"tha-ap1:63.4": "ratanā sabbe upenti maṃ.",
"tha-ap1:64.1": "Pathabyā pabbate ceva,",
"tha-ap1:64.2": "ākāse udake vane;",
"tha-ap1:64.3": "Yaṃ yaṃ hatthaṃ pasāremi,",
"tha-ap1:64.4": "sabbe gandhā upenti maṃ.",
"tha-ap1:65.1": "Pathabyā pabbate ceva,",
"tha-ap1:65.2": "ākāse udake vane;",
"tha-ap1:65.3": "Yaṃ yaṃ hatthaṃ pasāremi,",
"tha-ap1:65.4": "sabbe yānā upenti maṃ.",
"tha-ap1:66.1": "Pathabyā pabbate ceva,",
"tha-ap1:66.2": "ākāse udake vane;",
"tha-ap1:66.3": "Yaṃ yaṃ hatthaṃ pasāremi,",
"tha-ap1:66.4": "sabbe mālā upenti maṃ.",
"tha-ap1:67.1": "Pathabyā pabbate ceva,",
"tha-ap1:67.2": "ākāse udake vane;",
"tha-ap1:67.3": "Yaṃ yaṃ hatthaṃ pasāremi,",
"tha-ap1:67.4": "alaṅkārā upenti maṃ.",
"tha-ap1:68.1": "Pathabyā pabbate ceva,",
"tha-ap1:68.2": "ākāse udake vane;",
"tha-ap1:68.3": "Yaṃ yaṃ hatthaṃ pasāremi,",
"tha-ap1:68.4": "sabbā kaññā upenti maṃ.",
"tha-ap1:69.1": "Pathabyā pabbate ceva,",
"tha-ap1:69.2": "ākāse udake vane;",
"tha-ap1:69.3": "Yaṃ yaṃ hatthaṃ pasāremi,",
"tha-ap1:69.4": "madhusakkharā upenti maṃ.",
"tha-ap1:70.1": "Pathabyā pabbate ceva,",
"tha-ap1:70.2": "ākāse udake vane;",
"tha-ap1:70.3": "Yaṃ yaṃ hatthaṃ pasāremi,",
"tha-ap1:70.4": "sabbe khajjā upenti maṃ.",
"tha-ap1:71.1": "Adhane addhikajane,",
"tha-ap1:71.2": "yācake ca pathāvino;",
"tha-ap1:71.3": "Dadāmihaṃ dānavaraṃ,",
"tha-ap1:71.4": "sambodhivarapattiyā.",
"tha-ap1:72.1": "Nādento pabbataṃ selaṃ,",
"tha-ap1:72.2": "gajjento bahalaṃ giriṃ;",
"tha-ap1:72.3": "Sadevakaṃ hāsayanto,",
"tha-ap1:72.4": "buddho loke bhavāmahaṃ.",
"tha-ap1:73.1": "Disā dasavidhā loke,",
"tha-ap1:73.2": "yāyato natthi antakaṃ;",
"tha-ap1:73.3": "Tasmiñca disābhāgamhi,",
"tha-ap1:73.4": "buddhakhettā asaṅkhiyā.",
"tha-ap1:74.1": "Pabhā pakittitā mayhaṃ,",
"tha-ap1:74.2": "yamakā raṃsivāhanā;",
"tha-ap1:74.3": "Etthantare raṃsijālaṃ,",
"tha-ap1:74.4": "āloko vipulo bhave.",
"tha-ap1:75.1": "Ettake lokadhātumhi,",
"tha-ap1:75.2": "sabbe passantu maṃ janā;",
"tha-ap1:75.3": "Sabbe maṃ anuvattantu,",
"tha-ap1:75.4": "yāva brahmanivesanaṃ.",
"tha-ap1:76.1": "Visiṭṭhamadhunādena,",
"tha-ap1:76.2": "amatabherimāhaniṃ;",
"tha-ap1:76.3": "Etthantare janā sabbe,",
"tha-ap1:76.4": "suṇantu madhuraṃ giraṃ.",
"tha-ap1:77.1": "Dhammameghena vassante,",
"tha-ap1:77.2": "sabbe hontu anāsavā;",
"tha-ap1:77.3": "Yettha pacchimakā sattā,",
"tha-ap1:77.4": "sotāpannā bhavantu te.",
"tha-ap1:78.1": "Datvā dātabbakaṃ dānaṃ,",
"tha-ap1:78.2": "sīlaṃ pūretvā asesato;",
"tha-ap1:78.3": "Nekkhammapāramiṃ gantvā,",
"tha-ap1:78.4": "patto sambodhimuttamaṃ.",
"tha-ap1:79.1": "Paṇḍite paripucchitvā,",
"tha-ap1:79.2": "katvā vīriyamuttamaṃ;",
"tha-ap1:79.3": "Khantiyā pāramiṃ gantvā,",
"tha-ap1:79.4": "patto sambodhimuttamaṃ.",
"tha-ap1:80.1": "Katvā daḷhamadhiṭṭhānaṃ,",
"tha-ap1:80.2": "saccapārami pūriya;",
"tha-ap1:80.3": "Mettāya pāramiṃ gantvā,",
"tha-ap1:80.4": "patto sambodhimuttamaṃ.",
"tha-ap1:81.1": "Lābhālābhe sukhe dukkhe,",
"tha-ap1:81.2": "sammāne cāvamānane;",
"tha-ap1:81.3": "Sabbattha samako hutvā,",
"tha-ap1:81.4": "patto sambodhimuttamaṃ.",
"tha-ap1:82.1": "Kosajjaṃ bhayato disvā,",
"tha-ap1:82.2": "vīriyañcāpi khemato;",
"tha-ap1:82.3": "Āraddhavīriyā hotha,",
"tha-ap1:82.4": "esā buddhānusāsanī.",
"tha-ap1:83.1": "Vivādaṃ bhayato disvā,",
"tha-ap1:83.2": "avivādañca khemato;",
"tha-ap1:83.3": "Samaggā sakhilā hotha,",
"tha-ap1:83.4": "esā buddhānusāsanī.",
"tha-ap1:84.1": "Pamādaṃ bhayato disvā,",
"tha-ap1:84.2": "appamādañca khemato;",
"tha-ap1:84.3": "Bhāvethaṭṭhaṅgikaṃ maggaṃ,",
"tha-ap1:84.4": "esā buddhānusāsanī.",
"tha-ap1:85.1": "Samāgatā bahū buddhā,",
"tha-ap1:85.2": "arahantā ca sabbaso;",
"tha-ap1:85.3": "Sambuddhe arahante ca,",
"tha-ap1:85.4": "vandamānā namassatha.",
"tha-ap1:86.1": "Evaṃ acintiyā buddhā,",
"tha-ap1:86.2": "buddhadhammā acintiyā;",
"tha-ap1:86.3": "Acintiye pasannānaṃ,",
"tha-ap1:86.4": "vipāko hoti acintiyo”.",
"tha-ap1:87.1": "Itthaṃ sudaṃ bhagavā attano buddhacariyaṃ sambhāvayamāno buddhāpadāniyaṃ nāma dhammapariyāyaṃ abhāsitthāti.",
"tha-ap1:88.1": "Buddhāpadānaṃ samattaṃ."
}