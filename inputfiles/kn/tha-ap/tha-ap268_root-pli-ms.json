{
  "tha-ap268:0.1": "Therāpadāna ",
  "tha-ap268:0.2": "Padumukkhipavagga ",
  "tha-ap268:0.3": "6. Macchadāyakattheraapadāna ",
  "tha-ap268:1.1": "“Candabhāgānadītīre, ",
  "tha-ap268:1.2": "ukkuso āsahaṁ tadā; ",
  "tha-ap268:1.3": "Mahantaṁ macchaṁ paggayha, ",
  "tha-ap268:1.4": "siddhatthamunino adaṁ. ",
  "tha-ap268:2.1": "Catunnavutito kappe, ",
  "tha-ap268:2.2": "yaṁ macchamadadiṁ tadā; ",
  "tha-ap268:2.3": "Duggatiṁ nābhijānāmi, ",
  "tha-ap268:2.4": "macchadānassidaṁ phalaṁ. ",
  "tha-ap268:3.1": "Paṭisambhidā catasso, ",
  "tha-ap268:3.2": "…pe… ",
  "tha-ap268:3.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap268:4.1": "Itthaṁ sudaṁ āyasmā macchadāyako thero imā gāthāyo abhāsitthāti. ",
  "tha-ap268:5.1": "Macchadāyakattherassāpadānaṁ chaṭṭhaṁ. "
}