{
"tha-ap297:1.1": "Therāpadāna",
"tha-ap297:2.1": "Citakapūjakavagga",
"tha-ap297:3.1": "5. Gosīsanikkhepakattheraapadāna",
"tha-ap297:4.1": "“Ārāmadvārā nikkhamma,",
"tha-ap297:4.2": "gosīsaṃ santhataṃ mayā;",
"tha-ap297:4.3": "Anubhomi sakaṃ kammaṃ,",
"tha-ap297:4.4": "pubbakammassidaṃ phalaṃ.",
"tha-ap297:5.1": "Ājāniyā vātajavā,",
"tha-ap297:5.2": "sindhavā sīghavāhanā;",
"tha-ap297:5.3": "Anubhomi sabbametaṃ,",
"tha-ap297:5.4": "gosīsassa idaṃ phalaṃ.",
"tha-ap297:6.1": "Aho kāraṃ paramakāraṃ,",
"tha-ap297:6.2": "sukhette sukataṃ mayā;",
"tha-ap297:6.3": "Saṃghe katassa kārassa,",
"tha-ap297:6.4": "na aññaṃ kalamagghati.",
"tha-ap297:7.1": "Catunnavutito kappe,",
"tha-ap297:7.2": "yaṃ sīsaṃ santhariṃ ahaṃ;",
"tha-ap297:7.3": "Duggatiṃ nābhijānāmi,",
"tha-ap297:7.4": "santharassa idaṃ phalaṃ.",
"tha-ap297:8.1": "Pañcasattatikappamhi,",
"tha-ap297:8.2": "suppatiṭṭhitanāmako;",
"tha-ap297:8.3": "Eko āsiṃ mahātejo,",
"tha-ap297:8.4": "cakkavattī mahabbalo.",
"tha-ap297:9.1": "Paṭisambhidā catasso, … pe …",
"tha-ap297:9.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap297:10.1": "Itthaṃ sudaṃ āyasmā gosīsanikkhepako thero imā gāthāyo abhāsitthāti.",
"tha-ap297:11.1": "Gosīsanikkhepakattherassāpadānaṃ pañcamaṃ."
}