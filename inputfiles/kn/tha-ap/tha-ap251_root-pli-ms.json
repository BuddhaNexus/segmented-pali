{
"tha-ap251:1.1": "Therāpadāna",
"tha-ap251:2.1": "Tuvaradāyakavagga",
"tha-ap251:3.1": "9 Bimbijāliyattheraapadāna",
"tha-ap251:4.1": "“Padumuttaro nāma jino,",
"tha-ap251:4.2": "sayambhū aggapuggalo;",
"tha-ap251:4.3": "Catusaccaṃ pakāseti,",
"tha-ap251:4.4": "dīpeti amataṃ padaṃ.",
"tha-ap251:5.1": "Bimbijālakapupphāni,",
"tha-ap251:5.2": "puthu katvānahaṃ tadā;",
"tha-ap251:5.3": "Buddhassa abhiropesiṃ,",
"tha-ap251:5.4": "dvipadindassa tādino.",
"tha-ap251:6.1": "Aṭṭhasaṭṭhimhito kappe,",
"tha-ap251:6.2": "caturo kiñjakesarā;",
"tha-ap251:6.3": "Sattaratanasampannā,",
"tha-ap251:6.4": "cakkavattī mahabbalā.",
"tha-ap251:7.1": "Paṭisambhidā catasso, … pe …",
"tha-ap251:7.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap251:8.1": "Itthaṃ sudaṃ āyasmā bimbijāliyo thero imā gāthāyo abhāsitthāti.",
"tha-ap251:9.1": "Bimbijāliyattherassāpadānaṃ navamaṃ."
}