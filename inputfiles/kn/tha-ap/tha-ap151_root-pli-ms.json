{
"tha-ap151:1.1": "Therāpadāna",
"tha-ap151:2.1": "Chattavagga",
"tha-ap151:3.1": "9. Vaṭaṃsakiyattheraapadāna",
"tha-ap151:4.1": "“Sumedho nāma nāmena,",
"tha-ap151:4.2": "sayambhū aparājito;",
"tha-ap151:4.3": "Vivekamanubrūhanto,",
"tha-ap151:4.4": "ajjhogāhi mahāvanaṃ.",
"tha-ap151:5.1": "Saḷalaṃ pupphitaṃ disvā,",
"tha-ap151:5.2": "ganthitvāna vaṭaṃsakaṃ;",
"tha-ap151:5.3": "Buddhassa abhiropesiṃ,",
"tha-ap151:5.4": "sammukhā lokanāyakaṃ.",
"tha-ap151:6.1": "Tiṃsakappasahassamhi,",
"tha-ap151:6.2": "yaṃ pupphamabhiropayiṃ;",
"tha-ap151:6.3": "Duggatiṃ nābhijānāmi,",
"tha-ap151:6.4": "buddhapūjāyidaṃ phalaṃ.",
"tha-ap151:7.1": "Ūnavīse kappasate,",
"tha-ap151:7.2": "soḷasāsuṃ sunimmitā;",
"tha-ap151:7.3": "Sattaratanasampannā,",
"tha-ap151:7.4": "cakkavattī mahabbalā.",
"tha-ap151:8.1": "Paṭisambhidā catasso, … pe …",
"tha-ap151:8.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap151:9.1": "Itthaṃ sudaṃ āyasmā vaṭaṃsakiyo thero imā gāthāyo abhāsitthāti.",
"tha-ap151:10.1": "Vaṭaṃsakiyattherassāpadānaṃ navamaṃ."
}