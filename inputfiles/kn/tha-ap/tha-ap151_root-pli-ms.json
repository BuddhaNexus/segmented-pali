{
  "tha-ap151:0.1": "Therāpadāna ",
  "tha-ap151:0.2": "Chattavagga ",
  "tha-ap151:0.3": "9. Vaṭaṁsakiyattheraapadāna ",
  "tha-ap151:1.1": "“Sumedho nāma nāmena, ",
  "tha-ap151:1.2": "sayambhū aparājito; ",
  "tha-ap151:1.3": "Vivekamanubrūhanto, ",
  "tha-ap151:1.4": "ajjhogāhi mahāvanaṁ. ",
  "tha-ap151:2.1": "Saḷalaṁ pupphitaṁ disvā, ",
  "tha-ap151:2.2": "ganthitvāna vaṭaṁsakaṁ; ",
  "tha-ap151:2.3": "Buddhassa abhiropesiṁ, ",
  "tha-ap151:2.4": "sammukhā lokanāyakaṁ. ",
  "tha-ap151:3.1": "Tiṁsakappasahassamhi, ",
  "tha-ap151:3.2": "yaṁ pupphamabhiropayiṁ; ",
  "tha-ap151:3.3": "Duggatiṁ nābhijānāmi, ",
  "tha-ap151:3.4": "buddhapūjāyidaṁ phalaṁ. ",
  "tha-ap151:4.1": "Ūnavīse kappasate, ",
  "tha-ap151:4.2": "soḷasāsuṁ sunimmitā; ",
  "tha-ap151:4.3": "Sattaratanasampannā, ",
  "tha-ap151:4.4": "cakkavattī mahabbalā. ",
  "tha-ap151:5.1": "Paṭisambhidā catasso, ",
  "tha-ap151:5.2": "…pe… ",
  "tha-ap151:5.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap151:6.1": "Itthaṁ sudaṁ āyasmā vaṭaṁsakiyo thero imā gāthāyo abhāsitthāti. ",
  "tha-ap151:7.1": "Vaṭaṁsakiyattherassāpadānaṁ navamaṁ. "
}