{
  "tha-ap165:0.1": "Therāpadāna ",
  "tha-ap165:0.2": "Supāricariyavagga ",
  "tha-ap165:0.3": "3. Khajjakadāyakattheraapadāna ",
  "tha-ap165:1.1": "“Tissassa kho bhagavato, ",
  "tha-ap165:1.2": "pubbe phalamadāsahaṁ; ",
  "tha-ap165:1.3": "Nāḷikerañca pādāsiṁ, ",
  "tha-ap165:1.4": "khajjakaṁ abhisammataṁ. ",
  "tha-ap165:2.1": "Buddhassa tamahaṁ datvā, ",
  "tha-ap165:2.2": "Tissassa tu mahesino; ",
  "tha-ap165:2.3": "Modāmahaṁ kāmakāmī, ",
  "tha-ap165:2.4": "Upapajjiṁ yamicchakaṁ. ",
  "tha-ap165:3.1": "Dvenavute ito kappe, ",
  "tha-ap165:3.2": "yaṁ dānamadadiṁ tadā; ",
  "tha-ap165:3.3": "Duggatiṁ nābhijānāmi, ",
  "tha-ap165:3.4": "phaladānassidaṁ phalaṁ. ",
  "tha-ap165:4.1": "Ito terasakappamhi, ",
  "tha-ap165:4.2": "rājā indasamo ahu; ",
  "tha-ap165:4.3": "Sattaratanasampanno, ",
  "tha-ap165:4.4": "cakkavattī mahabbalo. ",
  "tha-ap165:5.1": "Paṭisambhidā catasso, ",
  "tha-ap165:5.2": "…pe… ",
  "tha-ap165:5.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap165:6.1": "Itthaṁ sudaṁ āyasmā khajjakadāyako thero imā gāthāyo abhāsitthāti. ",
  "tha-ap165:7.1": "Khajjakadāyakattherassāpadānaṁ tatiyaṁ. "
}