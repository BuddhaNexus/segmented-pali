{
"tha-ap134:1.1": "Therāpadāna",
"tha-ap134:2.1": "Sobhitavagga",
"tha-ap134:3.1": "2. Sudassanattheraapadāna",
"tha-ap134:4.1": "“Vinatā nadiyā tīre,",
"tha-ap134:4.2": "pilakkhu phalito ahu;",
"tha-ap134:4.3": "Tāhaṃ rukkhaṃ gavesanto,",
"tha-ap134:4.4": "addasaṃ lokanāyakaṃ.",
"tha-ap134:5.1": "Ketakaṃ pupphitaṃ disvā,",
"tha-ap134:5.2": "vaṇṭe chetvānahaṃ tadā;",
"tha-ap134:5.3": "Buddhassa abhiropesiṃ,",
"tha-ap134:5.4": "sikhino lokabandhuno.",
"tha-ap134:6.1": "Yena ñāṇena pattosi,",
"tha-ap134:6.2": "accutaṃ amataṃ padaṃ;",
"tha-ap134:6.3": "Taṃ ñāṇaṃ abhipūjemi,",
"tha-ap134:6.4": "buddhaseṭṭha mahāmuni.",
"tha-ap134:7.1": "Ñāṇamhi pūjaṃ katvāna,",
"tha-ap134:7.2": "pilakkhumaddasaṃ ahaṃ;",
"tha-ap134:7.3": "Paṭiladdhomhi taṃ paññaṃ,",
"tha-ap134:7.4": "ñāṇapūjāyidaṃ phalaṃ.",
"tha-ap134:8.1": "Ekattiṃse ito kappe,",
"tha-ap134:8.2": "yaṃ pupphamabhiropayiṃ;",
"tha-ap134:8.3": "Duggatiṃ nābhijānāmi,",
"tha-ap134:8.4": "ñāṇapūjāyidaṃ phalaṃ.",
"tha-ap134:9.1": "Ito terasakappamhi,",
"tha-ap134:9.2": "dvādasāsuṃ phaluggatā;",
"tha-ap134:9.3": "Sattaratanasampannā,",
"tha-ap134:9.4": "cakkavattī mahapphalā.",
"tha-ap134:10.1": "Paṭisambhidā catasso, … pe …",
"tha-ap134:10.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap134:11.1": "Itthaṃ sudaṃ āyasmā sudassano thero imā gāthāyo abhāsitthāti.",
"tha-ap134:12.1": "Sudassanattherassāpadānaṃ dutiyaṃ."
}