{
"tha-ap186:1.1": "Therāpadāna",
"tha-ap186:2.1": "Kuṭajapupphiyavagga",
"tha-ap186:3.1": "4. Pañcahatthiyattheraapadāna",
"tha-ap186:4.1": "“Tisso nāmāsi bhagavā,",
"tha-ap186:4.2": "lokajeṭṭho narāsabho;",
"tha-ap186:4.3": "Purakkhato sāvakehi,",
"tha-ap186:4.4": "rathiyaṃ paṭipajjatha.",
"tha-ap186:5.1": "Pañca uppalahatthā ca,",
"tha-ap186:5.2": "cāturā ṭhapitā mayā;",
"tha-ap186:5.3": "Āhutiṃ dātukāmohaṃ,",
"tha-ap186:5.4": "paggaṇhiṃ vatasiddhiyā.",
"tha-ap186:6.1": "Suvaṇṇavaṇṇaṃ sambuddhaṃ,",
"tha-ap186:6.2": "Gacchantaṃ antarāpaṇe;",
"tha-ap186:6.3": "Buddharaṃsīhi phuṭṭhosmi,",
"tha-ap186:6.4": "Pūjesiṃ dvipaduttamaṃ.",
"tha-ap186:7.1": "Dvenavute ito kappe,",
"tha-ap186:7.2": "yaṃ pupphamabhipūjayiṃ;",
"tha-ap186:7.3": "Duggatiṃ nābhijānāmi,",
"tha-ap186:7.4": "buddhapūjāyidaṃ phalaṃ.",
"tha-ap186:8.1": "Ito terasakappamhi,",
"tha-ap186:8.2": "pañca susabhasammatā;",
"tha-ap186:8.3": "Sattaratanasampannā,",
"tha-ap186:8.4": "cakkavattī mahabbalā.",
"tha-ap186:9.1": "Paṭisambhidā catasso, … pe …",
"tha-ap186:9.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap186:10.1": "Itthaṃ sudaṃ āyasmā pañcahatthiyo thero imā gāthāyo abhāsitthāti.",
"tha-ap186:11.1": "Pañcahatthiyattherassāpadānaṃ catutthaṃ."
}