{
"tha-ap482:1.1": "Therāpadāna",
"tha-ap482:2.1": "Naḷamālivagga",
"tha-ap482:3.1": "10 Pulinacaṅkamiyattheraapadāna",
"tha-ap482:4.1": "“Migaluddo pure āsiṃ,",
"tha-ap482:4.2": "araññe kānane ahaṃ;",
"tha-ap482:4.3": "Vātamigaṃ gavesanto,",
"tha-ap482:4.4": "caṅkamaṃ addasaṃ ahaṃ.",
"tha-ap482:5.1": "Ucchaṅgena pulinaṃ gayha,",
"tha-ap482:5.2": "caṅkame okiriṃ ahaṃ;",
"tha-ap482:5.3": "Pasannacitto sumano,",
"tha-ap482:5.4": "sugatassa sirīmato.",
"tha-ap482:6.1": "Ekatiṃse ito kappe,",
"tha-ap482:6.2": "pulinaṃ okiriṃ ahaṃ;",
"tha-ap482:6.3": "Duggatiṃ nābhijānāmi,",
"tha-ap482:6.4": "pulinassa idaṃ phalaṃ.",
"tha-ap482:7.1": "Kilesā jhāpitā mayhaṃ, … pe …",
"tha-ap482:7.4": "viharāmi anāsavo.",
"tha-ap482:8.1": "Svāgataṃ vata me āsi, … pe …",
"tha-ap482:8.4": "kataṃ buddhassa sāsanaṃ.",
"tha-ap482:9.1": "Paṭisambhidā catasso, … pe …",
"tha-ap482:9.4": "kataṃ buddhassa sāsanaṃ”. #4938",
"tha-ap482:10.1": "Itthaṃ sudaṃ āyasmā pulinacaṅkamiyo thero imā gāthāyo abhāsitthāti.",
"tha-ap482:11.1": "Pulinacaṅkamiyattherassāpadānaṃ dasamaṃ.",
"tha-ap482:12.1": "Naḷamālivaggo aṭṭhacattālīsamo.",
"tha-ap482:13.1": "Tassuddānaṃ",
"tha-ap482:14.1": "Naḷamālī maṇidado,",
"tha-ap482:14.2": "ukkāsatikabījanī;",
"tha-ap482:14.3": "Kummāso ca kusaṭṭho ca,",
"tha-ap482:14.4": "giripunnāgiyopi ca.",
"tha-ap482:15.1": "Vallikāro pānadhido,",
"tha-ap482:15.2": "atho pulinacaṅkamo;",
"tha-ap482:15.3": "Gāthāyo pañcanavuti,",
"tha-ap482:15.4": "gaṇitāyo vibhāvibhi."
}