{
"tha-ap129:1.1": "Therāpadāna",
"tha-ap129:2.1": "Sereyyavagga",
"tha-ap129:3.1": "7. Phaladāyakattheraapadāna",
"tha-ap129:4.1": "“Ajjhāyako mantadharo,",
"tha-ap129:4.2": "tiṇṇaṃ vedāna pāragū;",
"tha-ap129:4.3": "Himavantassāvidūre,",
"tha-ap129:4.4": "vasāmi assame ahaṃ.",
"tha-ap129:5.1": "Aggihuttañca me atthi,",
"tha-ap129:5.2": "puṇḍarīkaphalāni ca;",
"tha-ap129:5.3": "Puṭake nikkhipitvāna,",
"tha-ap129:5.4": "dumagge laggitaṃ mayā.",
"tha-ap129:6.1": "Padumuttaro lokavidū,",
"tha-ap129:6.2": "āhutīnaṃ paṭiggaho;",
"tha-ap129:6.3": "Mamuddharitukāmo so,",
"tha-ap129:6.4": "bhikkhanto mamupāgami.",
"tha-ap129:7.1": "Pasannacitto sumano,",
"tha-ap129:7.2": "phalaṃ buddhassadāsahaṃ;",
"tha-ap129:7.3": "Vittisañjanano mayhaṃ,",
"tha-ap129:7.4": "diṭṭhadhammasukhāvaho.",
"tha-ap129:8.1": "Suvaṇṇavaṇṇo sambuddho,",
"tha-ap129:8.2": "āhutīnaṃ paṭiggaho;",
"tha-ap129:8.3": "Antalikkhe ṭhito satthā,",
"tha-ap129:8.4": "imaṃ gāthaṃ abhāsatha.",
"tha-ap129:9.1": "‘Iminā phaladānena,",
"tha-ap129:9.2": "cetanāpaṇidhīhi ca;",
"tha-ap129:9.3": "Kappānaṃ satasahassaṃ,",
"tha-ap129:9.4": "duggatiṃ nupapajjati’.",
"tha-ap129:10.1": "Teneva sukkamūlena,",
"tha-ap129:10.2": "anubhotvāna sampadā;",
"tha-ap129:10.3": "Pattomhi acalaṃ ṭhānaṃ,",
"tha-ap129:10.4": "hitvā jayaparājayaṃ.",
"tha-ap129:11.1": "Ito sattasate kappe,",
"tha-ap129:11.2": "rājā āsiṃ sumaṅgalo;",
"tha-ap129:11.3": "Sattaratanasampanno,",
"tha-ap129:11.4": "cakkavattī mahabbalo.",
"tha-ap129:12.1": "Paṭisambhidā catasso, … pe …",
"tha-ap129:12.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap129:13.1": "Itthaṃ sudaṃ āyasmā phaladāyako thero imā gāthāyo abhāsitthāti.",
"tha-ap129:14.1": "Phaladāyakattherassāpadānaṃ sattamaṃ."
}