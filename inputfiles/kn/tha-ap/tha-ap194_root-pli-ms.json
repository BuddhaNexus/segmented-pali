{
"tha-ap194:1.1": "Therāpadāna",
"tha-ap194:2.1": "Tamālapupphiyavagga",
"tha-ap194:3.1": "2 Tiṇasanthārakattheraapadāna",
"tha-ap194:4.1": "“Yaṃ dāyavāsiko isi,",
"tha-ap194:4.2": "tiṇaṃ lāyati satthuno;",
"tha-ap194:4.3": "Sabbe padakkhiṇāvaṭṭā,",
"tha-ap194:4.4": "pathabyā nipatiṃsu te.",
"tha-ap194:5.1": "Tamahaṃ tiṇamādāya,",
"tha-ap194:5.2": "santhariṃ dharaṇuttame;",
"tha-ap194:5.3": "Tīṇeva tālapattāni,",
"tha-ap194:5.4": "āharitvānahaṃ tadā.",
"tha-ap194:6.1": "Tiṇena chadanaṃ katvā,",
"tha-ap194:6.2": "siddhatthassa adāsahaṃ;",
"tha-ap194:6.3": "Sattāhaṃ dhārayuṃ tassa,",
"tha-ap194:6.4": "devamānusasatthuno.",
"tha-ap194:7.1": "Catunnavutito kappe,",
"tha-ap194:7.2": "yaṃ tiṇaṃ adadiṃ tadā;",
"tha-ap194:7.3": "Duggatiṃ nābhijānāmi,",
"tha-ap194:7.4": "tiṇadānassidaṃ phalaṃ.",
"tha-ap194:8.1": "Pañcasaṭṭhimhito kappe,",
"tha-ap194:8.2": "cattārosuṃ mahaddhanā;",
"tha-ap194:8.3": "Sattaratanasampannā,",
"tha-ap194:8.4": "cakkavattī mahabbalā.",
"tha-ap194:9.1": "Paṭisambhidā catasso, … pe …",
"tha-ap194:9.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap194:10.1": "Itthaṃ sudaṃ āyasmā tiṇasanthārako thero imā gāthāyo abhāsitthāti.",
"tha-ap194:11.1": "Tiṇasanthārakattherassāpadānaṃ dutiyaṃ."
}