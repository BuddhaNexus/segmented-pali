{
"tha-ap451:1.1": "Therāpadāna",
"tha-ap451:2.1": "Vibhītakavagga",
"tha-ap451:3.1": "9 Vedikārakattheraapadāna",
"tha-ap451:4.1": "“Padumuttarassa bhagavato,",
"tha-ap451:4.2": "Bodhiyā pādaputtame;",
"tha-ap451:4.3": "Vedikaṃ sukataṃ katvā,",
"tha-ap451:4.4": "Sakaṃ cittaṃ pasādayiṃ.",
"tha-ap451:5.1": "Atoḷārāni bhaṇḍāni,",
"tha-ap451:5.2": "katāni akatāni ca;",
"tha-ap451:5.3": "Antalikkhā pavassanti,",
"tha-ap451:5.4": "vedikāya idaṃ phalaṃ.",
"tha-ap451:6.1": "Ubhato byūḷhasaṅgāme,",
"tha-ap451:6.2": "pakkhandanto bhayānake;",
"tha-ap451:6.3": "Bhayabheravaṃ na passāmi,",
"tha-ap451:6.4": "vedikāya idaṃ phalaṃ.",
"tha-ap451:7.1": "Mama saṅkappamaññāya,",
"tha-ap451:7.2": "byamhaṃ nibbattate subhaṃ;",
"tha-ap451:7.3": "Sayanāni mahagghāni,",
"tha-ap451:7.4": "vedikāya idaṃ phalaṃ.",
"tha-ap451:8.1": "Satasahassito kappe,",
"tha-ap451:8.2": "yaṃ vedikamakārayiṃ;",
"tha-ap451:8.3": "Duggatiṃ nābhijānāmi,",
"tha-ap451:8.4": "vedikāya idaṃ phalaṃ.",
"tha-ap451:9.1": "Kilesā jhāpitā mayhaṃ, … pe …",
"tha-ap451:9.4": "viharāmi anāsavo.",
"tha-ap451:10.1": "Svāgataṃ vata me āsi, … pe …",
"tha-ap451:10.4": "kataṃ buddhassa sāsanaṃ.",
"tha-ap451:11.1": "Paṭisambhidā catasso, … pe …",
"tha-ap451:11.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap451:12.1": "Itthaṃ sudaṃ āyasmā vedikārako thero imā gāthāyo abhāsitthāti.",
"tha-ap451:13.1": "Vedikārakattherassāpadānaṃ navamaṃ."
}