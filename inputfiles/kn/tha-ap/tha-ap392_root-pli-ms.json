{
"tha-ap392:1.1": "Therāpadāna",
"tha-ap392:2.1": "Avaṭaphalavagga",
"tha-ap392:3.1": "10 Pubbakammapilotikabuddhaapadāna",
"tha-ap392:4.1": "Anotattasarāsanne,",
"tha-ap392:4.2": "ramaṇīye silātale;",
"tha-ap392:4.3": "Nānāratanapajjote,",
"tha-ap392:4.4": "nānāgandhavanantare.",
"tha-ap392:5.1": "Mahatā bhikkhusaṃghena,",
"tha-ap392:5.2": "pareto lokanāyako;",
"tha-ap392:5.3": "Āsīno byākarī tattha,",
"tha-ap392:5.4": "pubbakammāni attano.",
"tha-ap392:6.1": "“Suṇātha bhikkhavo mayhaṃ,",
"tha-ap392:6.2": "yaṃ kammaṃ pakataṃ mayā;",
"tha-ap392:6.3": "Pilotikassa kammassa,",
"tha-ap392:6.4": "buddhattepi vipaccati.",
"tha-ap392:7.1": "10.1",
"tha-ap392:8.1": "Munāḷi nāmahaṃ dhutto,",
"tha-ap392:8.2": "pubbe aññāsu jātisu;",
"tha-ap392:8.3": "Paccekabuddhaṃ surabhiṃ,",
"tha-ap392:8.4": "abbhācikkhiṃ adūsakaṃ.",
"tha-ap392:9.1": "Tena kammavipākena,",
"tha-ap392:9.2": "niraye saṃsariṃ ciraṃ;",
"tha-ap392:9.3": "Bahūvassasahassāni,",
"tha-ap392:9.4": "dukkhaṃ vedesi vedanaṃ.",
"tha-ap392:10.1": "Tena kammāvasesena,",
"tha-ap392:10.2": "idha pacchimake bhave;",
"tha-ap392:10.3": "Abbhakkhānaṃ mayā laddhaṃ,",
"tha-ap392:10.4": "sundarikāya kāraṇā.",
"tha-ap392:11.1": "10.2",
"tha-ap392:12.1": "Sabbābhibhussa buddhassa,",
"tha-ap392:12.2": "nando nāmāsi sāvako;",
"tha-ap392:12.3": "Taṃ abbhakkhāya niraye,",
"tha-ap392:12.4": "ciraṃ saṃsaritaṃ mayā.",
"tha-ap392:13.1": "Dasavassasahassāni,",
"tha-ap392:13.2": "niraye saṃsariṃ ciraṃ;",
"tha-ap392:13.3": "Manussabhāvaṃ laddhāhaṃ,",
"tha-ap392:13.4": "abbhakkhānaṃ bahuṃ labhiṃ.",
"tha-ap392:14.1": "Tena kammāvasesena,",
"tha-ap392:14.2": "ciñcamānavikā mamaṃ;",
"tha-ap392:14.3": "Abbhācikkhi abhūtena,",
"tha-ap392:14.4": "janakāyassa aggato.",
"tha-ap392:15.1": "10.3",
"tha-ap392:16.1": "Brāhmaṇo sutavā āsiṃ,",
"tha-ap392:16.2": "ahaṃ sakkatapūjito;",
"tha-ap392:16.3": "Mahāvane pañcasate,",
"tha-ap392:16.4": "mante vācemi māṇave.",
"tha-ap392:17.1": "Tatthāgato isi bhīmo,",
"tha-ap392:17.2": "pañcābhiñño mahiddhiko;",
"tha-ap392:17.3": "Tañcāhaṃ āgataṃ disvā,",
"tha-ap392:17.4": "abbhācikkhiṃ adūsakaṃ.",
"tha-ap392:18.1": "Tatohaṃ avacaṃ sisse,",
"tha-ap392:18.2": "kāmabhogī ayaṃ isi;",
"tha-ap392:18.3": "Mayhampi bhāsamānassa,",
"tha-ap392:18.4": "anumodiṃsu māṇavā.",
"tha-ap392:19.1": "Tato māṇavakā sabbe,",
"tha-ap392:19.2": "bhikkhamānaṃ kule kule;",
"tha-ap392:19.3": "Mahājanassa āhaṃsu,",
"tha-ap392:19.4": "kāmabhogī ayaṃ isi.",
"tha-ap392:20.1": "Tena kammavipākena,",
"tha-ap392:20.2": "pañca bhikkhusatā ime;",
"tha-ap392:20.3": "Abbhakkhānaṃ labhuṃ sabbe,",
"tha-ap392:20.4": "sundarikāya kāraṇā.",
"tha-ap392:21.1": "10.4",
"tha-ap392:22.1": "Vemātubhātaraṃ pubbe,",
"tha-ap392:22.2": "dhanahetu haniṃ ahaṃ;",
"tha-ap392:22.3": "Pakkhipiṃ giriduggasmiṃ,",
"tha-ap392:22.4": "silāya ca apiṃsayiṃ.",
"tha-ap392:23.1": "Tena kammavipākena,",
"tha-ap392:23.2": "devadatto silaṃ khipi;",
"tha-ap392:23.3": "Aṅguṭṭhaṃ piṃsayī pāde,",
"tha-ap392:23.4": "mama pāsāṇasakkharā.",
"tha-ap392:24.1": "10.5",
"tha-ap392:25.1": "Purehaṃ dārako hutvā,",
"tha-ap392:25.2": "kīḷamāno mahāpathe;",
"tha-ap392:25.3": "Paccekabuddhaṃ disvāna,",
"tha-ap392:25.4": "magge sakalikaṃ khipiṃ.",
"tha-ap392:26.1": "Tena kammavipākena,",
"tha-ap392:26.2": "idha pacchimake bhave;",
"tha-ap392:26.3": "Vadhatthaṃ maṃ devadatto,",
"tha-ap392:26.4": "abhimāre payojayi.",
"tha-ap392:27.1": "10.6",
"tha-ap392:28.1": "Hatthāroho pure āsiṃ,",
"tha-ap392:28.2": "paccekamunimuttamaṃ;",
"tha-ap392:28.3": "Piṇḍāya vicarantaṃ taṃ,",
"tha-ap392:28.4": "āsādesiṃ gajenahaṃ.",
"tha-ap392:29.1": "Tena kammavipākena,",
"tha-ap392:29.2": "bhanto nāḷāgirī gajo;",
"tha-ap392:29.3": "Giribbaje puravare,",
"tha-ap392:29.4": "dāruṇo samupāgami.",
"tha-ap392:30.1": "10.7",
"tha-ap392:31.1": "Rājāhaṃ patthivo āsiṃ,",
"tha-ap392:31.2": "sattiyā purisaṃ haniṃ;",
"tha-ap392:31.3": "Tena kammavipākena,",
"tha-ap392:31.4": "niraye paccisaṃ bhusaṃ.",
"tha-ap392:32.1": "Kammuno tassa sesena,",
"tha-ap392:32.2": "idāni sakalaṃ mama;",
"tha-ap392:32.3": "Pāde chaviṃ pakappesi,",
"tha-ap392:32.4": "na hi kammaṃ vinassati.",
"tha-ap392:33.1": "10.8",
"tha-ap392:34.1": "Ahaṃ kevaṭṭagāmasmiṃ,",
"tha-ap392:34.2": "ahuṃ kevaṭṭadārako;",
"tha-ap392:34.3": "Macchake ghātite disvā,",
"tha-ap392:34.4": "janayiṃ somanassakaṃ.",
"tha-ap392:35.1": "Tena kammavipākena,",
"tha-ap392:35.2": "sīsadukkhaṃ ahū mama;",
"tha-ap392:35.3": "Sabbe sakkā ca haññiṃsu,",
"tha-ap392:35.4": "yadā hani viṭaṭūbho.",
"tha-ap392:36.1": "10.9",
"tha-ap392:37.1": "Phussassāhaṃ pāvacane,",
"tha-ap392:37.2": "sāvake paribhāsayiṃ;",
"tha-ap392:37.3": "Yavaṃ khādatha bhuñjatha,",
"tha-ap392:37.4": "mā ca bhuñjatha sālayo.",
"tha-ap392:38.1": "Tena kammavipākena,",
"tha-ap392:38.2": "temāsaṃ khāditaṃ yavaṃ;",
"tha-ap392:38.3": "Nimantito brāhmaṇena,",
"tha-ap392:38.4": "verañjāyaṃ vasiṃ tadā.",
"tha-ap392:39.1": "10.10",
"tha-ap392:40.1": "Nibbuddhe vattamānamhi,",
"tha-ap392:40.2": "mallaputtaṃ niheṭhayiṃ;",
"tha-ap392:40.3": "Tena kammavipākena,",
"tha-ap392:40.4": "piṭṭhidukkhaṃ ahū mama.",
"tha-ap392:41.1": "10.11",
"tha-ap392:42.1": "Tikicchako ahaṃ āsiṃ,",
"tha-ap392:42.2": "seṭṭhiputtaṃ virecayiṃ;",
"tha-ap392:42.3": "Tena kammavipākena,",
"tha-ap392:42.4": "hoti pakkhandikā mama.",
"tha-ap392:43.1": "10.12",
"tha-ap392:44.1": "Avacāhaṃ jotipālo,",
"tha-ap392:44.2": "sugataṃ kassapaṃ tadā;",
"tha-ap392:44.3": "Kuto nu bodhi muṇḍassa,",
"tha-ap392:44.4": "bodhi paramadullabhā.",
"tha-ap392:45.1": "Tena kammavipākena,",
"tha-ap392:45.2": "acariṃ dukkaraṃ bahuṃ;",
"tha-ap392:45.3": "Chabbassānuruvelāyaṃ,",
"tha-ap392:45.4": "tato bodhimapāpuṇiṃ.",
"tha-ap392:46.1": "Nāhaṃ etena maggena,",
"tha-ap392:46.2": "pāpuṇiṃ bodhimuttamaṃ;",
"tha-ap392:46.3": "Kummaggena gavesissaṃ,",
"tha-ap392:46.4": "pubbakammena vārito.",
"tha-ap392:47.1": "Puññapāpaparikkhīṇo,",
"tha-ap392:47.2": "sabbasantāpavajjito;",
"tha-ap392:47.3": "Asoko anupāyāso,",
"tha-ap392:47.4": "nibbāyissamanāsavo”.",
"tha-ap392:48.1": "Evaṃ jino viyākāsi,",
"tha-ap392:48.2": "Bhikkhusaṃghassa aggato;",
"tha-ap392:48.3": "Sabbābhiññābalappatto,",
"tha-ap392:48.4": "Anotatte mahāsareti. #3367",
"tha-ap392:49.1": "Itthaṃ sudaṃ bhagavā attano pubbacaritaṃ kammapilotikaṃ nāma buddhāpadānadhammapariyāyaṃ abhāsitthāti.",
"tha-ap392:50.1": "Pubbakammapilotikaṃ nāma buddhāpadānaṃ dasamaṃ.",
"tha-ap392:51.1": "Avaṭaphalavaggo ekūnacattālīsamo.",
"tha-ap392:52.1": "Tassuddānaṃ",
"tha-ap392:53.1": "Avaṭaṃ labujañceva,",
"tha-ap392:53.2": "udumbarapilakkhu ca;",
"tha-ap392:53.3": "Phāru vallī ca kadalī,",
"tha-ap392:53.4": "panaso koṭivīsako.",
"tha-ap392:54.1": "Pubbakammapiloti ca,",
"tha-ap392:54.2": "apadānaṃ mahesino;",
"tha-ap392:54.3": "Gāthāyo ekanavuti,",
"tha-ap392:54.4": "gaṇitāyo vibhāvibhi.",
"tha-ap392:55.1": "Cuddasamaṃ bhāṇavāraṃ."
}