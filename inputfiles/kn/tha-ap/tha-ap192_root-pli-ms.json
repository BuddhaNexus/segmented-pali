{
"tha-ap192:1.1": "Therāpadāna",
"tha-ap192:2.1": "Kuṭajapupphiyavagga",
"tha-ap192:3.1": "10. Tivaṇṭipupphiyattheraapadāna",
"tha-ap192:4.1": "“Abhibhūtaṃ panijjhanti,",
"tha-ap192:4.2": "Sabbe saṅgamma te mamaṃ;",
"tha-ap192:4.3": "Tesaṃ nijjhāyamānānaṃ,",
"tha-ap192:4.4": "Pariḷāho ajāyatha.",
"tha-ap192:5.1": "Sunando nāma nāmena,",
"tha-ap192:5.2": "buddhassa sāvako tadā;",
"tha-ap192:5.3": "Dhammadassissa munino,",
"tha-ap192:5.4": "āgacchi mama santikaṃ.",
"tha-ap192:6.1": "Ye me baddhacarā āsuṃ,",
"tha-ap192:6.2": "te me pupphaṃ aduṃ tadā;",
"tha-ap192:6.3": "Tāhaṃ pupphaṃ gahetvāna,",
"tha-ap192:6.4": "sāvake abhiropayiṃ.",
"tha-ap192:7.1": "Sohaṃ kālaṅkato tattha,",
"tha-ap192:7.2": "punāpi upapajjahaṃ;",
"tha-ap192:7.3": "Aṭṭhārase kappasate,",
"tha-ap192:7.4": "vinipātaṃ na gacchahaṃ.",
"tha-ap192:8.1": "Teraseto kappasate,",
"tha-ap192:8.2": "aṭṭhāsuṃ dhūmaketuno;",
"tha-ap192:8.3": "Sattaratanasampannā,",
"tha-ap192:8.4": "cakkavattī mahabbalā.",
"tha-ap192:9.1": "Paṭisambhidā catasso, … pe …",
"tha-ap192:9.4": "kataṃ buddhassa sāsanaṃ”. #2208",
"tha-ap192:10.1": "Itthaṃ sudaṃ āyasmā tivaṇṭipupphiyo thero imā gāthāyo abhāsitthāti.",
"tha-ap192:11.1": "Tivaṇṭipupphiyattherassāpadānaṃ dasamaṃ.",
"tha-ap192:12.1": "Kuṭajapupphiyavaggo ekūnavīsatimo.",
"tha-ap192:13.1": "Tassuddānaṃ",
"tha-ap192:14.1": "Kuṭajo bandhujīvī ca,",
"tha-ap192:14.2": "koṭumbarikahatthiyo;",
"tha-ap192:14.3": "Isimuggo ca bodhi ca,",
"tha-ap192:14.4": "ekacintī tikaṇṇiko;",
"tha-ap192:14.5": "Ekacārī tivaṇṭi ca,",
"tha-ap192:14.6": "gāthā dvāsaṭṭhi kittitāti."
}