{
"tha-ap27:1.1": "Therāpadāna",
"tha-ap27:2.1": "Subhūtivagga",
"tha-ap27:3.1": "5. Annasaṃsāvakattheraapadāna",
"tha-ap27:4.1": "“Suvaṇṇavaṇṇaṃ sambuddhaṃ,",
"tha-ap27:4.2": "Gacchantaṃ antarāpaṇe;",
"tha-ap27:4.3": "Kañcanagghiyasaṃkāsaṃ,",
"tha-ap27:4.4": "Bāttiṃsavaralakkhaṇaṃ.",
"tha-ap27:5.1": "Siddhatthaṃ lokapajjotaṃ,",
"tha-ap27:5.2": "appameyyaṃ anopamaṃ;",
"tha-ap27:5.3": "Alatthaṃ paramaṃ pītiṃ,",
"tha-ap27:5.4": "disvā dantaṃ jutindharaṃ.",
"tha-ap27:6.1": "Sambuddhaṃ abhināmetvā,",
"tha-ap27:6.2": "bhojayiṃ taṃ mahāmuniṃ;",
"tha-ap27:6.3": "Mahākāruṇiko loke,",
"tha-ap27:6.4": "anumodi mamaṃ tadā.",
"tha-ap27:7.1": "Tasmiṃ mahākāruṇike,",
"tha-ap27:7.2": "paramassāsakārake;",
"tha-ap27:7.3": "Buddhe cittaṃ pasādetvā,",
"tha-ap27:7.4": "kappaṃ saggamhi modahaṃ.",
"tha-ap27:8.1": "Catunnavutito kappe,",
"tha-ap27:8.2": "yaṃ dānamadadiṃ tadā;",
"tha-ap27:8.3": "Duggatiṃ nābhijānāmi,",
"tha-ap27:8.4": "bhikkhādānassidaṃ phalaṃ.",
"tha-ap27:9.1": "Paṭisambhidā catasso,",
"tha-ap27:9.2": "vimokkhāpi ca aṭṭhime;",
"tha-ap27:9.3": "Chaḷabhiññā sacchikatā,",
"tha-ap27:9.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap27:10.1": "Itthaṃ sudaṃ āyasmā annasaṃsāvako thero imā gāthāyo abhāsitthāti.",
"tha-ap27:11.1": "Annasaṃsāvakattherassāpadānaṃ pañcamaṃ."
}