{
"tha-ap139:1.1": "Therāpadāna",
"tha-ap139:2.1": "Sobhitavagga",
"tha-ap139:3.1": "7. Atthasandassakattheraapadāna",
"tha-ap139:4.1": "“Visālamāḷe āsīno,",
"tha-ap139:4.2": "addasaṃ lokanāyakaṃ;",
"tha-ap139:4.3": "Khīṇāsavaṃ balappattaṃ,",
"tha-ap139:4.4": "bhikkhusaṃghapurakkhataṃ.",
"tha-ap139:5.1": "Satasahassā tevijjā,",
"tha-ap139:5.2": "chaḷabhiññā mahiddhikā;",
"tha-ap139:5.3": "Parivārenti sambuddhaṃ,",
"tha-ap139:5.4": "ko disvā nappasīdati.",
"tha-ap139:6.1": "Ñāṇe upanidhā yassa,",
"tha-ap139:6.2": "na vijjati sadevake;",
"tha-ap139:6.3": "Anantañāṇaṃ sambuddhaṃ,",
"tha-ap139:6.4": "ko disvā nappasīdati.",
"tha-ap139:7.1": "Dhammakāyañca dīpentaṃ,",
"tha-ap139:7.2": "kevalaṃ ratanākaraṃ;",
"tha-ap139:7.3": "Vikappetuṃ na sakkonti,",
"tha-ap139:7.4": "ko disvā nappasīdati.",
"tha-ap139:8.1": "Imāhi tīhi gāthāhi,",
"tha-ap139:8.2": "nāradovhayavacchalo;",
"tha-ap139:8.3": "Padumuttaraṃ thavitvāna,",
"tha-ap139:8.4": "sambuddhaṃ aparājitaṃ.",
"tha-ap139:9.1": "Tena cittappasādena,",
"tha-ap139:9.2": "buddhasanthavanena ca;",
"tha-ap139:9.3": "Kappānaṃ satasahassaṃ,",
"tha-ap139:9.4": "duggatiṃ nupapajjahaṃ.",
"tha-ap139:10.1": "Ito tiṃsakappasate,",
"tha-ap139:10.2": "sumitto nāma khattiyo;",
"tha-ap139:10.3": "Sattaratanasampanno,",
"tha-ap139:10.4": "cakkavattī mahabbalo.",
"tha-ap139:11.1": "Paṭisambhidā catasso, … pe …",
"tha-ap139:11.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap139:12.1": "Itthaṃ sudaṃ āyasmā atthasandassako thero imā gāthāyo abhāsitthāti.",
"tha-ap139:13.1": "Atthasandassakattherassāpadānaṃ sattamaṃ."
}