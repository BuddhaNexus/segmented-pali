{
"tha-ap242:1.1": "Therāpadāna",
"tha-ap242:2.1": "Udakāsanavagga",
"tha-ap242:3.1": "10. Vāsidāyakattheraapadāna",
"tha-ap242:4.1": "“Kammārohaṃ pure āsiṃ,",
"tha-ap242:4.2": "tivarāyaṃ puruttame;",
"tha-ap242:4.3": "Ekā vāsi mayā dinnā,",
"tha-ap242:4.4": "sayambhuṃ aparājitaṃ.",
"tha-ap242:5.1": "Catunnavutito kappe,",
"tha-ap242:5.2": "yaṃ vāsimadadiṃ tadā;",
"tha-ap242:5.3": "Duggatiṃ nābhijānāmi,",
"tha-ap242:5.4": "vāsidānassidaṃ phalaṃ.",
"tha-ap242:6.1": "Paṭisambhidā catasso, … pe …",
"tha-ap242:6.4": "kataṃ buddhassa sāsanaṃ”. #2457",
"tha-ap242:7.1": "Itthaṃ sudaṃ āyasmā vāsidāyako thero imā gāthāyo abhāsitthāti.",
"tha-ap242:8.1": "Vāsidāyakattherassāpadānaṃ dasamaṃ.",
"tha-ap242:9.1": "Udakāsanavaggo catuvīsatimo.",
"tha-ap242:10.1": "Tassuddānaṃ",
"tha-ap242:11.1": "Udakāsanabhājanaṃ,",
"tha-ap242:11.2": "sālapupphī kilañjako;",
"tha-ap242:11.3": "Vedikā vaṇṇakāro ca,",
"tha-ap242:11.4": "piyālaambayāgado;",
"tha-ap242:11.5": "Jagatī vāsidātā ca,",
"tha-ap242:11.6": "gāthā tiṃsa ca aṭṭha ca."
}