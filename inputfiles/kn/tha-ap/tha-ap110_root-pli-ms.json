{
"tha-ap110:1.1": "Therāpadāna",
"tha-ap110:2.1": "Bhikkhadāyivagga",
"tha-ap110:3.1": "8 Āsanupaṭṭhāhakattheraapadāna",
"tha-ap110:4.1": "“Kānanaṃ vanamogayha,",
"tha-ap110:4.2": "appasaddaṃ nirākulaṃ;",
"tha-ap110:4.3": "Sīhāsanaṃ mayā dinnaṃ,",
"tha-ap110:4.4": "atthadassissa tādino.",
"tha-ap110:5.1": "Mālāhatthaṃ gahetvāna,",
"tha-ap110:5.2": "katvā ca naṃ padakkhiṇaṃ;",
"tha-ap110:5.3": "Satthāraṃ payirupāsitvā,",
"tha-ap110:5.4": "pakkāmiṃ uttarāmukho.",
"tha-ap110:6.1": "Tena kammena dvipadinda,",
"tha-ap110:6.2": "lokajeṭṭha narāsabha;",
"tha-ap110:6.3": "Sannibbāpemi attānaṃ,",
"tha-ap110:6.4": "bhavā sabbe samūhatā.",
"tha-ap110:7.1": "Aṭṭhārasakappasate,",
"tha-ap110:7.2": "yaṃ dānamadadiṃ tadā;",
"tha-ap110:7.3": "Duggatiṃ nābhijānāmi,",
"tha-ap110:7.4": "sīhāsanassidaṃ phalaṃ.",
"tha-ap110:8.1": "Ito sattakappasate,",
"tha-ap110:8.2": "sannibbāpaka khattiyo;",
"tha-ap110:8.3": "Sattaratanasampanno,",
"tha-ap110:8.4": "cakkavattī mahabbalo.",
"tha-ap110:9.1": "Paṭisambhidā catasso, … pe …",
"tha-ap110:9.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap110:10.1": "Itthaṃ sudaṃ āyasmā āsanupaṭṭhāhako thero imā gāthāyo abhāsitthāti.",
"tha-ap110:11.1": "Āsanupaṭṭhāhakattherassāpadānaṃ aṭṭhamaṃ."
}