{
"tha-ap190:1.1": "Therāpadāna",
"tha-ap190:2.1": "Kuṭajapupphiyavagga",
"tha-ap190:3.1": "8. Tikaṇṇipupphiyattheraapadāna",
"tha-ap190:4.1": "“Devabhūto ahaṃ santo,",
"tha-ap190:4.2": "accharāhi purakkhato;",
"tha-ap190:4.3": "Pubbakammaṃ saritvāna,",
"tha-ap190:4.4": "buddhaseṭṭhaṃ anussariṃ.",
"tha-ap190:5.1": "Tikaṇṇipupphaṃ paggayha,",
"tha-ap190:5.2": "sakaṃ cittaṃ pasādayiṃ;",
"tha-ap190:5.3": "Buddhamhi abhiropesiṃ,",
"tha-ap190:5.4": "vipassimhi narāsabhe.",
"tha-ap190:6.1": "Ekanavutito kappe,",
"tha-ap190:6.2": "yaṃ pupphamabhipūjayiṃ;",
"tha-ap190:6.3": "Duggatiṃ nābhijānāmi,",
"tha-ap190:6.4": "buddhapūjāyidaṃ phalaṃ.",
"tha-ap190:7.1": "Tesattatimhito kappe,",
"tha-ap190:7.2": "caturāsuṃ ramuttamā;",
"tha-ap190:7.3": "Sattaratanasampannā,",
"tha-ap190:7.4": "cakkavattī mahabbalā.",
"tha-ap190:8.1": "Paṭisambhidā catasso, … pe …",
"tha-ap190:8.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap190:9.1": "Itthaṃ sudaṃ āyasmā tikaṇṇipupphiyo thero imā gāthāyo abhāsitthāti.",
"tha-ap190:10.1": "Tikaṇṇipupphiyattherassāpadānaṃ aṭṭhamaṃ."
}