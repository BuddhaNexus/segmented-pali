{
  "tha-ap200:0.1": "Therāpadāna ",
  "tha-ap200:0.2": "Tamālapupphiyavagga ",
  "tha-ap200:0.3": "8 Muṭṭhipūjakattheraapadāna ",
  "tha-ap200:1.1": "“Sumedho nāma bhagavā, ",
  "tha-ap200:1.2": "lokajeṭṭho narāsabho; ",
  "tha-ap200:1.3": "Pacchime anukampāya, ",
  "tha-ap200:1.4": "padhānaṁ padahī jino. ",
  "tha-ap200:2.1": "Tassa caṅkamamānassa, ",
  "tha-ap200:2.2": "dvipadindassa tādino; ",
  "tha-ap200:2.3": "Girinelassa pupphānaṁ, ",
  "tha-ap200:2.4": "muṭṭhiṁ buddhassa ropayiṁ. ",
  "tha-ap200:3.1": "Tena cittappasādena, ",
  "tha-ap200:3.2": "sukkamūlena codito; ",
  "tha-ap200:3.3": "Tiṁsakappasahassāni, ",
  "tha-ap200:3.4": "duggatiṁ nupapajjahaṁ. ",
  "tha-ap200:4.1": "Tevīsatikappasate, ",
  "tha-ap200:4.2": "sunelo nāma khattiyo; ",
  "tha-ap200:4.3": "Sattaratanasampanno, ",
  "tha-ap200:4.4": "eko āsiṁ mahabbalo. ",
  "tha-ap200:5.1": "Paṭisambhidā catasso, ",
  "tha-ap200:5.2": "…pe… ",
  "tha-ap200:5.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap200:6.1": "Itthaṁ sudaṁ āyasmā muṭṭhipūjako thero imā gāthāyo abhāsitthāti. ",
  "tha-ap200:7.1": "Muṭṭhipūjakattherassāpadānaṁ aṭṭhamaṁ. "
}