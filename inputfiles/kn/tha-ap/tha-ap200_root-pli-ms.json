{
"tha-ap200:1.1": "Therāpadāna",
"tha-ap200:2.1": "Tamālapupphiyavagga",
"tha-ap200:3.1": "8 Muṭṭhipūjakattheraapadāna",
"tha-ap200:4.1": "“Sumedho nāma bhagavā,",
"tha-ap200:4.2": "lokajeṭṭho narāsabho;",
"tha-ap200:4.3": "Pacchime anukampāya,",
"tha-ap200:4.4": "padhānaṃ padahī jino.",
"tha-ap200:5.1": "Tassa caṅkamamānassa,",
"tha-ap200:5.2": "dvipadindassa tādino;",
"tha-ap200:5.3": "Girinelassa pupphānaṃ,",
"tha-ap200:5.4": "muṭṭhiṃ buddhassa ropayiṃ.",
"tha-ap200:6.1": "Tena cittappasādena,",
"tha-ap200:6.2": "sukkamūlena codito;",
"tha-ap200:6.3": "Tiṃsakappasahassāni,",
"tha-ap200:6.4": "duggatiṃ nupapajjahaṃ.",
"tha-ap200:7.1": "Tevīsatikappasate,",
"tha-ap200:7.2": "sunelo nāma khattiyo;",
"tha-ap200:7.3": "Sattaratanasampanno,",
"tha-ap200:7.4": "eko āsiṃ mahabbalo.",
"tha-ap200:8.1": "Paṭisambhidā catasso, … pe …",
"tha-ap200:8.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap200:9.1": "Itthaṃ sudaṃ āyasmā muṭṭhipūjako thero imā gāthāyo abhāsitthāti.",
"tha-ap200:10.1": "Muṭṭhipūjakattherassāpadānaṃ aṭṭhamaṃ."
}