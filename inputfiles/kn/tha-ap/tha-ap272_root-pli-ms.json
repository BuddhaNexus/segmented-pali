{
"tha-ap272:1.1": "Therāpadāna",
"tha-ap272:2.1": "Padumukkhipavagga",
"tha-ap272:3.1": "10. Taraṇiyattheraapadāna",
"tha-ap272:4.1": "“Suvaṇṇavaṇṇo sambuddho,",
"tha-ap272:4.2": "vipassī dakkhiṇāraho;",
"tha-ap272:4.3": "Nadītīre ṭhito satthā,",
"tha-ap272:4.4": "bhikkhusaṃghapurakkhato.",
"tha-ap272:5.1": "Nāvā na vijjate tattha,",
"tha-ap272:5.2": "santāraṇī mahaṇṇave;",
"tha-ap272:5.3": "Nadiyā abhinikkhamma,",
"tha-ap272:5.4": "tāresiṃ lokanāyakaṃ.",
"tha-ap272:6.1": "Ekanavutito kappe,",
"tha-ap272:6.2": "yaṃ tāresiṃ naruttamaṃ;",
"tha-ap272:6.3": "Duggatiṃ nābhijānāmi,",
"tha-ap272:6.4": "taraṇāya idaṃ phalaṃ.",
"tha-ap272:7.1": "Paṭisambhidā catasso, … pe …",
"tha-ap272:7.4": "kataṃ buddhassa sāsanaṃ”. #2577",
"tha-ap272:8.1": "Itthaṃ sudaṃ āyasmā taraṇiyo thero imā gāthāyo abhāsitthāti.",
"tha-ap272:9.1": "Taraṇiyattherassāpadānaṃ dasamaṃ.",
"tha-ap272:10.1": "Padumukkhipavaggo sattavīsatimo.",
"tha-ap272:11.1": "Tassuddānaṃ",
"tha-ap272:12.1": "Ukkhipī telacandī ca,",
"tha-ap272:12.2": "dīpado ca biḷālido;",
"tha-ap272:12.3": "Maccho javo saḷalado,",
"tha-ap272:12.4": "rakkhaso taraṇo dasa;",
"tha-ap272:12.5": "Gāthāyo cettha saṅkhātā,",
"tha-ap272:12.6": "tālīsaṃ cekameva cāti."
}