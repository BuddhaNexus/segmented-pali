{
"tha-ap66:1.1": "Therāpadāna",
"tha-ap66:2.1": "Sakacintaniyavagga",
"tha-ap66:3.1": "4. Parappasādakattheraapadāna",
"tha-ap66:4.1": "“Usabhaṃ pavaraṃ vīraṃ,",
"tha-ap66:4.2": "Mahesiṃ vijitāvinaṃ;",
"tha-ap66:4.3": "Suvaṇṇavaṇṇaṃ sambuddhaṃ,",
"tha-ap66:4.4": "Ko disvā nappasīdati.",
"tha-ap66:5.1": "Himavāvāparimeyyo,",
"tha-ap66:5.2": "sāgarova duruttaro;",
"tha-ap66:5.3": "Tatheva jhānaṃ buddhassa,",
"tha-ap66:5.4": "ko disvā nappasīdati.",
"tha-ap66:6.1": "Vasudhā yathāppameyyā,",
"tha-ap66:6.2": "cittā vanavaṭaṃsakā;",
"tha-ap66:6.3": "Tatheva sīlaṃ buddhassa,",
"tha-ap66:6.4": "ko disvā nappasīdati.",
"tha-ap66:7.1": "Anilañjasāsaṅkhubbho,",
"tha-ap66:7.2": "yathākāso asaṅkhiyo;",
"tha-ap66:7.3": "Tatheva ñāṇaṃ buddhassa,",
"tha-ap66:7.4": "ko disvā nappasīdati.",
"tha-ap66:8.1": "Imāhi catugāthāhi,",
"tha-ap66:8.2": "brāhmaṇo senasavhayo;",
"tha-ap66:8.3": "Buddhaseṭṭhaṃ thavitvāna,",
"tha-ap66:8.4": "siddhatthaṃ aparājitaṃ.",
"tha-ap66:9.1": "Catunnavutikappāni,",
"tha-ap66:9.2": "duggatiṃ nupapajjatha;",
"tha-ap66:9.3": "Sugatiṃ sukhasampattiṃ,",
"tha-ap66:9.4": "anubhosimanappakaṃ.",
"tha-ap66:10.1": "Catunnavutito kappe,",
"tha-ap66:10.2": "thavitvā lokanāyakaṃ;",
"tha-ap66:10.3": "Duggatiṃ nābhijānāmi,",
"tha-ap66:10.4": "thomanāya idaṃ phalaṃ.",
"tha-ap66:11.1": "Cātuddasamhi kappamhi,",
"tha-ap66:11.2": "caturo āsumuggatā;",
"tha-ap66:11.3": "Sattaratanasampannā,",
"tha-ap66:11.4": "cakkavattī mahabbalā.",
"tha-ap66:12.1": "Paṭisambhidā catasso, … pe …",
"tha-ap66:12.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap66:13.1": "Itthaṃ sudaṃ āyasmā parappasādako thero imā gāthāyo abhāsitthāti.",
"tha-ap66:14.1": "Parappasādakattherassāpadānaṃ catutthaṃ."
}