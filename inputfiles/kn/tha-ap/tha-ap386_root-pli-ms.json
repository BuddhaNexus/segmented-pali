{
  "tha-ap386:0.1": "Therāpadāna ",
  "tha-ap386:0.2": "Avaṭaphalavagga ",
  "tha-ap386:0.3": "4 Pilakkhaphaladāyakattheraapadāna ",
  "tha-ap386:1.1": "“Vanantare buddhaṁ disvā, ",
  "tha-ap386:1.2": "atthadassiṁ mahāyasaṁ; ",
  "tha-ap386:1.3": "Pasannacitto sumano, ",
  "tha-ap386:1.4": "pilakkhassādadiṁ phalaṁ. ",
  "tha-ap386:2.1": "Aṭṭhārase kappasate, ",
  "tha-ap386:2.2": "yaṁ phalamadadiṁ tadā; ",
  "tha-ap386:2.3": "Duggatiṁ nābhijānāmi, ",
  "tha-ap386:2.4": "phaladānassidaṁ phalaṁ. ",
  "tha-ap386:3.1": "Kilesā jhāpitā mayhaṁ, ",
  "tha-ap386:3.2": "…pe… ",
  "tha-ap386:3.3": "viharāmi anāsavo. ",
  "tha-ap386:4.1": "Svāgataṁ vata me āsi, ",
  "tha-ap386:4.2": "…pe… ",
  "tha-ap386:4.3": "kataṁ buddhassa sāsanaṁ. ",
  "tha-ap386:5.1": "Paṭisambhidā catasso, ",
  "tha-ap386:5.2": "…pe… ",
  "tha-ap386:5.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap386:6.1": "Itthaṁ sudaṁ āyasmā pilakkhaphaladāyako thero imā gāthāyo abhāsitthāti. ",
  "tha-ap386:7.1": "Pilakkhaphaladāyakattherassāpadānaṁ catutthaṁ. "
}