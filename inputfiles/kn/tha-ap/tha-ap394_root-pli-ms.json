{
"tha-ap394:1.1": "Therāpadāna",
"tha-ap394:2.1": "Pilindavacchavagga",
"tha-ap394:3.1": "2. Selattheraapadāna",
"tha-ap394:4.1": "“Nagare haṃsavatiyā,",
"tha-ap394:4.2": "vīthisāmī ahosahaṃ;",
"tha-ap394:4.3": "Mama ñātī samānetvā,",
"tha-ap394:4.4": "idaṃ vacanamabraviṃ.",
"tha-ap394:5.1": "‘Buddho loke samuppanno,",
"tha-ap394:5.2": "puññakkhetto anuttaro;",
"tha-ap394:5.3": "Āsi so sabbalokassa,",
"tha-ap394:5.4": "āhutīnaṃ paṭiggaho.",
"tha-ap394:6.1": "Khattiyā negamā ceva,",
"tha-ap394:6.2": "mahāsālā ca brāhmaṇā;",
"tha-ap394:6.3": "Pasannacittā sumanā,",
"tha-ap394:6.4": "pūgadhammaṃ akaṃsu te.",
"tha-ap394:7.1": "Hatthārohā anīkaṭṭhā,",
"tha-ap394:7.2": "rathikā pattikārakā;",
"tha-ap394:7.3": "Pasannacittā sumanā,",
"tha-ap394:7.4": "pūgadhammaṃ akaṃsu te.",
"tha-ap394:8.1": "Uggā ca rājaputtā ca,",
"tha-ap394:8.2": "vesiyānā ca brāhmaṇā;",
"tha-ap394:8.3": "Pasannacittā sumanā,",
"tha-ap394:8.4": "pūgadhammaṃ akaṃsu te.",
"tha-ap394:9.1": "Āḷārikā kappakā ca,",
"tha-ap394:9.2": "nhāpakā mālakārakā;",
"tha-ap394:9.3": "Pasannacittā sumanā,",
"tha-ap394:9.4": "pūgadhammaṃ akaṃsu te.",
"tha-ap394:10.1": "Rajakā pesakārā ca,",
"tha-ap394:10.2": "cammakārā ca nhāpitā;",
"tha-ap394:10.3": "Pasannacittā sumanā,",
"tha-ap394:10.4": "pūgadhammaṃ akaṃsu te.",
"tha-ap394:11.1": "Usukārā bhamakārā,",
"tha-ap394:11.2": "cammakārā ca tacchakā;",
"tha-ap394:11.3": "Pasannacittā sumanā,",
"tha-ap394:11.4": "pūgadhammaṃ akaṃsu te.",
"tha-ap394:12.1": "Kammārā soṇṇakārā ca,",
"tha-ap394:12.2": "tipulohakarā tathā;",
"tha-ap394:12.3": "Pasannacittā sumanā,",
"tha-ap394:12.4": "pūgadhammaṃ akaṃsu te.",
"tha-ap394:13.1": "Bhatakā ceṭakā ceva,",
"tha-ap394:13.2": "dāsakammakarā bahū;",
"tha-ap394:13.3": "Yathāsakena thāmena,",
"tha-ap394:13.4": "pūgadhammaṃ akaṃsu te.",
"tha-ap394:14.1": "Udahārā kaṭṭhahārā,",
"tha-ap394:14.2": "kassakā tiṇahārakā;",
"tha-ap394:14.3": "Yathāsakena thāmena,",
"tha-ap394:14.4": "pūgadhammaṃ akaṃsu te.",
"tha-ap394:15.1": "Pupphikā mālikā ceva,",
"tha-ap394:15.2": "paṇṇikā phalahārakā;",
"tha-ap394:15.3": "Yathāsakena thāmena,",
"tha-ap394:15.4": "pūgadhammaṃ akaṃsu te.",
"tha-ap394:16.1": "Gaṇikā kumbhadāsī ca,",
"tha-ap394:16.2": "pūvikā macchikāpi ca;",
"tha-ap394:16.3": "Yathāsakena thāmena,",
"tha-ap394:16.4": "pūgadhammaṃ akaṃsu te.",
"tha-ap394:17.1": "Etha sabbe samāgantvā,",
"tha-ap394:17.2": "gaṇaṃ bandhāma ekato;",
"tha-ap394:17.3": "Adhikāraṃ karissāma,",
"tha-ap394:17.4": "puññakkhette anuttare’.",
"tha-ap394:18.1": "Te me sutvāna vacanaṃ,",
"tha-ap394:18.2": "gaṇaṃ bandhiṃsu tāvade;",
"tha-ap394:18.3": "‘Upaṭṭhānasālaṃ sukataṃ,",
"tha-ap394:18.4": "bhikkhusaṃghassa kārayuṃ’.",
"tha-ap394:19.1": "Niṭṭhāpetvāna taṃ sālaṃ,",
"tha-ap394:19.2": "udaggo tuṭṭhamānaso;",
"tha-ap394:19.3": "Pareto tehi sabbehi,",
"tha-ap394:19.4": "sambuddhamupasaṅkamiṃ.",
"tha-ap394:20.1": "Upasaṅkamma sambuddhaṃ,",
"tha-ap394:20.2": "lokanāthaṃ narāsabhaṃ;",
"tha-ap394:20.3": "Vanditvā satthuno pāde,",
"tha-ap394:20.4": "idaṃ vacanamabraviṃ.",
"tha-ap394:21.1": "‘Ime tīṇi satā vīra,",
"tha-ap394:21.2": "Purisā ekato gaṇā;",
"tha-ap394:21.3": "Upaṭṭhānasālaṃ sukataṃ,",
"tha-ap394:21.4": "Niyyādenti tuvaṃ muni.",
"tha-ap394:22.1": "Bhikkhusaṃghassa purato,",
"tha-ap394:22.2": "sampaṭicchitva cakkhumā’;",
"tha-ap394:22.3": "Tiṇṇaṃ satānaṃ purato,",
"tha-ap394:22.4": "imā gāthā abhāsatha.",
"tha-ap394:23.1": "‘Tisatāpi ca jeṭṭho ca,",
"tha-ap394:23.2": "anuvattiṃsu ekato;",
"tha-ap394:23.3": "Sampattiñhi karitvāna,",
"tha-ap394:23.4": "sabbe anubhavissatha.",
"tha-ap394:24.1": "Pacchime bhave sampatte,",
"tha-ap394:24.2": "sītibhāvamanuttaraṃ;",
"tha-ap394:24.3": "Ajaraṃ amataṃ santaṃ,",
"tha-ap394:24.4": "nibbānaṃ phassayissatha’.",
"tha-ap394:25.1": "Evaṃ buddho viyākāsi,",
"tha-ap394:25.2": "sabbaññū samaṇuttaro;",
"tha-ap394:25.3": "Buddhassa vacanaṃ sutvā,",
"tha-ap394:25.4": "somanassaṃ pavedayiṃ.",
"tha-ap394:26.1": "Tiṃsa kappasahassāni,",
"tha-ap394:26.2": "devaloke ramiṃ ahaṃ;",
"tha-ap394:26.3": "Devādhipo pañcasataṃ,",
"tha-ap394:26.4": "devarajjamakārayiṃ.",
"tha-ap394:27.1": "Sahassakkhattuṃ rājā ca,",
"tha-ap394:27.2": "cakkavattī ahosahaṃ;",
"tha-ap394:27.3": "Devarajjaṃ karontassa,",
"tha-ap394:27.4": "mahādevā avandisuṃ.",
"tha-ap394:28.1": "Idha mānusake rajjaṃ,",
"tha-ap394:28.2": "parisā honti bandhavā;",
"tha-ap394:28.3": "Pacchime bhave sampatte,",
"tha-ap394:28.4": "vāseṭṭho nāma brāhmaṇo.",
"tha-ap394:29.1": "Asītikoṭi nicayo,",
"tha-ap394:29.2": "tassa putto ahosahaṃ;",
"tha-ap394:29.3": "Selo iti mamaṃ nāmaṃ,",
"tha-ap394:29.4": "chaḷaṅge pāramiṃ gato.",
"tha-ap394:30.1": "Jaṅghāvihāraṃ vicaraṃ,",
"tha-ap394:30.2": "sasissehi purakkhato;",
"tha-ap394:30.3": "Jaṭābhārikabharitaṃ,",
"tha-ap394:30.4": "keṇiyaṃ nāma tāpasaṃ.",
"tha-ap394:31.1": "Paṭiyattāhutiṃ disvā,",
"tha-ap394:31.2": "idaṃ vacanamabraviṃ;",
"tha-ap394:31.3": "‘Āvāho vā vivāho vā,",
"tha-ap394:31.4": "rājā vā te nimantito’.",
"tha-ap394:32.1": "‘Āhutiṃ yiṭṭhukāmohaṃ,",
"tha-ap394:32.2": "brāhmaṇe devasammate;",
"tha-ap394:32.3": "Na nimantemi rājānaṃ,",
"tha-ap394:32.4": "āhutī me na vijjati.",
"tha-ap394:33.1": "Na catthi mayhamāvāho,",
"tha-ap394:33.2": "vivāho me na vijjati;",
"tha-ap394:33.3": "Sakyānaṃ nandijanano,",
"tha-ap394:33.4": "seṭṭho loke sadevake.",
"tha-ap394:34.1": "Sabbalokahitatthāya,",
"tha-ap394:34.2": "sabbasattasukhāvaho;",
"tha-ap394:34.3": "So me nimantito ajja,",
"tha-ap394:34.4": "tassetaṃ paṭiyādanaṃ.",
"tha-ap394:35.1": "Timbarūsakavaṇṇābho,",
"tha-ap394:35.2": "appameyyo anūpamo;",
"tha-ap394:35.3": "Rūpenāsadiso buddho,",
"tha-ap394:35.4": "svātanāya nimantito.",
"tha-ap394:36.1": "Ukkāmukhapahaṭṭhova,",
"tha-ap394:36.2": "khadiraṅgārasannibho;",
"tha-ap394:36.3": "Vijjūpamo mahāvīro,",
"tha-ap394:36.4": "so me buddho nimantito.",
"tha-ap394:37.1": "Pabbatagge yathā acci,",
"tha-ap394:37.2": "puṇṇamāyeva candimā;",
"tha-ap394:37.3": "Naḷaggivaṇṇasaṅkāso,",
"tha-ap394:37.4": "so me buddho nimantito.",
"tha-ap394:38.1": "Asambhīto bhayātīto,",
"tha-ap394:38.2": "bhavantakaraṇo muni;",
"tha-ap394:38.3": "Sīhūpamo mahāvīro,",
"tha-ap394:38.4": "so me buddho nimantito.",
"tha-ap394:39.1": "Kusalo buddhadhammehi,",
"tha-ap394:39.2": "apasayho parehi so;",
"tha-ap394:39.3": "Nāgūpamo mahāvīro,",
"tha-ap394:39.4": "so me buddho nimantito.",
"tha-ap394:40.1": "Saddhammācārakusalo,",
"tha-ap394:40.2": "buddhanāgo asādiso;",
"tha-ap394:40.3": "Usabhūpamo mahāvīro,",
"tha-ap394:40.4": "so me buddho nimantito.",
"tha-ap394:41.1": "Anantavaṇṇo amitayaso,",
"tha-ap394:41.2": "Vicittasabbalakkhaṇo;",
"tha-ap394:41.3": "Sakkūpamo mahāvīro,",
"tha-ap394:41.4": "So me buddho nimantito.",
"tha-ap394:42.1": "Vasī gaṇī patāpī ca,",
"tha-ap394:42.2": "tejassī ca durāsado;",
"tha-ap394:42.3": "Brahmūpamo mahāvīro,",
"tha-ap394:42.4": "so me buddho nimantito.",
"tha-ap394:43.1": "Pattadhammo dasabalo,",
"tha-ap394:43.2": "balātibalapārago;",
"tha-ap394:43.3": "Dharaṇūpamo mahāvīro,",
"tha-ap394:43.4": "so me buddho nimantito.",
"tha-ap394:44.1": "Sīlavīcisamākiṇṇo,",
"tha-ap394:44.2": "dhammaviññāṇakhobhito;",
"tha-ap394:44.3": "Udadhūpamo mahāvīro,",
"tha-ap394:44.4": "so me buddho nimantito.",
"tha-ap394:45.1": "Durāsado duppasaho,",
"tha-ap394:45.2": "acalo uggato brahā;",
"tha-ap394:45.3": "Nerūpamo mahāvīro,",
"tha-ap394:45.4": "so me buddho nimantito.",
"tha-ap394:46.1": "Anantañāṇo asamasamo,",
"tha-ap394:46.2": "Atulo aggataṃ gato;",
"tha-ap394:46.3": "Gaganūpamo mahāvīro,",
"tha-ap394:46.4": "So me buddho nimantito.",
"tha-ap394:47.1": "Pannarasamaṃ bhāṇavāraṃ.",
"tha-ap394:48.1": "Patiṭṭhā bhayabhītānaṃ,",
"tha-ap394:48.2": "tāṇo saraṇagāminaṃ;",
"tha-ap394:48.3": "Assāsako mahāvīro,",
"tha-ap394:48.4": "so me buddho nimantito.",
"tha-ap394:49.1": "Āsayo buddhimantānaṃ,",
"tha-ap394:49.2": "puññakkhettaṃ sukhesinaṃ;",
"tha-ap394:49.3": "Ratanākaro mahāvīro,",
"tha-ap394:49.4": "so me buddho nimantito.",
"tha-ap394:50.1": "Assāsako vedakaro,",
"tha-ap394:50.2": "sāmaññaphaladāyako;",
"tha-ap394:50.3": "Meghūpamo mahāvīro,",
"tha-ap394:50.4": "so me buddho nimantito.",
"tha-ap394:51.1": "Lokacakkhu mahātejo,",
"tha-ap394:51.2": "sabbatamavinodano;",
"tha-ap394:51.3": "Sūriyūpamo mahāvīro,",
"tha-ap394:51.4": "so me buddho nimantito.",
"tha-ap394:52.1": "Ārammaṇavimuttīsu,",
"tha-ap394:52.2": "sabhāvadassano muni;",
"tha-ap394:52.3": "Candūpamo mahāvīro,",
"tha-ap394:52.4": "so me buddho nimantito.",
"tha-ap394:53.1": "Buddho samussito loke,",
"tha-ap394:53.2": "lakkhaṇehi alaṅkato;",
"tha-ap394:53.3": "Appameyyo mahāvīro,",
"tha-ap394:53.4": "so me buddho nimantito.",
"tha-ap394:54.1": "Yassa ñāṇaṃ appameyyaṃ,",
"tha-ap394:54.2": "sīlaṃ yassa anūpamaṃ;",
"tha-ap394:54.3": "Vimutti asadisā yassa,",
"tha-ap394:54.4": "so me buddho nimantito.",
"tha-ap394:55.1": "Yassa dhīti asadisā,",
"tha-ap394:55.2": "thāmo yassa acintiyo;",
"tha-ap394:55.3": "Yassa parakkamo jeṭṭho,",
"tha-ap394:55.4": "so me buddho nimantito.",
"tha-ap394:56.1": "Rāgo doso ca moho ca,",
"tha-ap394:56.2": "visā sabbe samūhatā;",
"tha-ap394:56.3": "Agadūpamo mahāvīro,",
"tha-ap394:56.4": "so me buddho nimantito.",
"tha-ap394:57.1": "Klesabyādhibahudukkha,",
"tha-ap394:57.2": "sabbatamavinodano;",
"tha-ap394:57.3": "Vejjūpamo mahāvīro,",
"tha-ap394:57.4": "so me buddho nimantito’.",
"tha-ap394:58.1": "Buddhoti bho yaṃ vadesi,",
"tha-ap394:58.2": "ghosopeso sudullabho;",
"tha-ap394:58.3": "Buddho buddhoti sutvāna,",
"tha-ap394:58.4": "pīti me udapajjatha.",
"tha-ap394:59.1": "Abbhantaraṃ agaṇhantaṃ,",
"tha-ap394:59.2": "pīti me bahi nicchare;",
"tha-ap394:59.3": "Sohaṃ pītimano santo,",
"tha-ap394:59.4": "idaṃ vacanamabraviṃ.",
"tha-ap394:60.1": "‘Kahaṃ nu kho so bhagavā,",
"tha-ap394:60.2": "lokajeṭṭho narāsabho;",
"tha-ap394:60.3": "Tattha gantvā namassissaṃ,",
"tha-ap394:60.4": "sāmaññaphaladāyakaṃ.",
"tha-ap394:61.1": "Paggayha dakkhiṇaṃ bāhuṃ,",
"tha-ap394:61.2": "vedajāto katañjalī;",
"tha-ap394:61.3": "Ācikkhi me dhammarājaṃ,",
"tha-ap394:61.4": "sokasallavinodanaṃ’.",
"tha-ap394:62.1": "‘Udentaṃva mahāmeghaṃ,",
"tha-ap394:62.2": "nīlaṃ añjanasannibhaṃ;",
"tha-ap394:62.3": "Sāgaraṃ viya dissantaṃ,",
"tha-ap394:62.4": "passasetaṃ mahāvanaṃ.",
"tha-ap394:63.1": "Ettha so vasate buddho,",
"tha-ap394:63.2": "adantadamako muni;",
"tha-ap394:63.3": "Vinayanto ca veneyye,",
"tha-ap394:63.4": "bodhento bodhipakkhiye’.",
"tha-ap394:64.1": "Pipāsitova udakaṃ,",
"tha-ap394:64.2": "bhojanaṃva jighacchito;",
"tha-ap394:64.3": "Gāvī yathā vacchagiddhā,",
"tha-ap394:64.4": "evāhaṃ viciniṃ jinaṃ.",
"tha-ap394:65.1": "Ācāraupacāraññū,",
"tha-ap394:65.2": "dhammānucchavisaṃvaraṃ;",
"tha-ap394:65.3": "Sikkhāpemi sake sisse,",
"tha-ap394:65.4": "gacchante jinasantikaṃ.",
"tha-ap394:66.1": "‘Durāsadā bhagavanto,",
"tha-ap394:66.2": "sīhāva ekacārino;",
"tha-ap394:66.3": "Pade padaṃ nikkhipantā,",
"tha-ap394:66.4": "āgaccheyyātha māṇavā.",
"tha-ap394:67.1": "Āsīviso yathā ghoro,",
"tha-ap394:67.2": "migarājāva kesarī;",
"tha-ap394:67.3": "Mattova kuñjaro dantī,",
"tha-ap394:67.4": "evaṃ buddhā durāsadā.",
"tha-ap394:68.1": "Ukkāsitañca khipitaṃ,",
"tha-ap394:68.2": "ajjhupekkhiya māṇavā;",
"tha-ap394:68.3": "Pade padaṃ nikkhipantā,",
"tha-ap394:68.4": "upetha buddhasantikaṃ.",
"tha-ap394:69.1": "Paṭisallānagarukā,",
"tha-ap394:69.2": "appasaddā durāsadā;",
"tha-ap394:69.3": "Durūpasaṅkamā buddhā,",
"tha-ap394:69.4": "garū honti sadevake.",
"tha-ap394:70.1": "Yadāhaṃ pañhaṃ pucchāmi,",
"tha-ap394:70.2": "paṭisammodayāmi vā;",
"tha-ap394:70.3": "Appasaddā tadā hotha,",
"tha-ap394:70.4": "munibhūtāva tiṭṭhatha.",
"tha-ap394:71.1": "Yaṃ so deseti sambuddho,",
"tha-ap394:71.2": "khemaṃ nibbānapattiyā;",
"tha-ap394:71.3": "Tamevatthaṃ nisāmetha,",
"tha-ap394:71.4": "saddhammasavanaṃ sukhaṃ’.",
"tha-ap394:72.1": "Upasaṅkamma sambuddhaṃ,",
"tha-ap394:72.2": "sammodiṃ muninā ahaṃ;",
"tha-ap394:72.3": "Taṃ kathaṃ vītisāretvā,",
"tha-ap394:72.4": "lakkhaṇe upadhārayiṃ.",
"tha-ap394:73.1": "Lakkhaṇe dve ca kaṅkhāmi,",
"tha-ap394:73.2": "passāmi tiṃsalakkhaṇe;",
"tha-ap394:73.3": "Kosohitavatthaguyhaṃ,",
"tha-ap394:73.4": "iddhiyā dassayī muni.",
"tha-ap394:74.1": "Jivhaṃ ninnāmayitvāna,",
"tha-ap394:74.2": "kaṇṇasote ca nāsike;",
"tha-ap394:74.3": "Paṭimasi nalāṭantaṃ,",
"tha-ap394:74.4": "kevalaṃ chādayī jino.",
"tha-ap394:75.1": "Tassāhaṃ lakkhaṇe disvā,",
"tha-ap394:75.2": "paripuṇṇe sabyañjane;",
"tha-ap394:75.3": "Buddhoti niṭṭhaṃ gantvāna,",
"tha-ap394:75.4": "saha sissehi pabbajiṃ.",
"tha-ap394:76.1": "Satehi tīhi sahito,",
"tha-ap394:76.2": "pabbajiṃ anagāriyaṃ;",
"tha-ap394:76.3": "Addhamāse asampatte,",
"tha-ap394:76.4": "sabbe pattāmha nibbutiṃ.",
"tha-ap394:77.1": "Ekato kammaṃ katvāna,",
"tha-ap394:77.2": "puññakkhette anuttare;",
"tha-ap394:77.3": "Ekato saṃsaritvāna,",
"tha-ap394:77.4": "ekato vinivattayuṃ.",
"tha-ap394:78.1": "Gopānasiyo datvāna,",
"tha-ap394:78.2": "pūgadhamme vasiṃ ahaṃ;",
"tha-ap394:78.3": "Tena kammena sukatena,",
"tha-ap394:78.4": "aṭṭha hetū labhāmahaṃ.",
"tha-ap394:79.1": "Disāsu pūjito homi,",
"tha-ap394:79.2": "bhogā ca amitā mama;",
"tha-ap394:79.3": "Patiṭṭhā homi sabbesaṃ,",
"tha-ap394:79.4": "tāso mama na vijjati.",
"tha-ap394:80.1": "Byādhayo me na vijjanti,",
"tha-ap394:80.2": "dīghāyuṃ pālayāmi ca;",
"tha-ap394:80.3": "Sukhumacchaviko homi,",
"tha-ap394:80.4": "āvāse patthite vase.",
"tha-ap394:81.1": "Aṭṭha gopānasī datvā,",
"tha-ap394:81.2": "pūgadhamme vasiṃ ahaṃ;",
"tha-ap394:81.3": "Paṭisambhidārahattañca,",
"tha-ap394:81.4": "etaṃ me aparaṭṭhamaṃ.",
"tha-ap394:82.1": "Sabbavositavosāno,",
"tha-ap394:82.2": "katakicco anāsavo;",
"tha-ap394:82.3": "Aṭṭhagopānasī nāma,",
"tha-ap394:82.4": "tava putto mahāmuni.",
"tha-ap394:83.1": "Pañca thambhāni datvāna,",
"tha-ap394:83.2": "pūgadhamme vasiṃ ahaṃ;",
"tha-ap394:83.3": "Tena kammena sukatena,",
"tha-ap394:83.4": "pañca hetū labhāmahaṃ.",
"tha-ap394:84.1": "Acalo homi mettāya,",
"tha-ap394:84.2": "anūnaṅgo bhavāmahaṃ;",
"tha-ap394:84.3": "Ādeyyavacano homi,",
"tha-ap394:84.4": "na dhaṃsemi yathā ahaṃ.",
"tha-ap394:85.1": "Abhantaṃ hoti me cittaṃ,",
"tha-ap394:85.2": "akhilo homi kassaci;",
"tha-ap394:85.3": "Tena kammena sukatena,",
"tha-ap394:85.4": "vimalo homi sāsane.",
"tha-ap394:86.1": "Sagāravo sappatisso,",
"tha-ap394:86.2": "katakicco anāsavo;",
"tha-ap394:86.3": "Sāvako te mahāvīra,",
"tha-ap394:86.4": "bhikkhu taṃ vandate muni.",
"tha-ap394:87.1": "Katvā sukatapallaṅkaṃ,",
"tha-ap394:87.2": "sālāyaṃ paññapesahaṃ;",
"tha-ap394:87.3": "Tena kammena sukatena,",
"tha-ap394:87.4": "pañca hetū labhāmahaṃ.",
"tha-ap394:88.1": "Ucce kule pajāyitvā,",
"tha-ap394:88.2": "mahābhogo bhavāmahaṃ;",
"tha-ap394:88.3": "Sabbasampattiko homi,",
"tha-ap394:88.4": "maccheraṃ me na vijjati.",
"tha-ap394:89.1": "Gamane patthite mayhaṃ,",
"tha-ap394:89.2": "pallaṅko upatiṭṭhati;",
"tha-ap394:89.3": "Saha pallaṅkaseṭṭhena,",
"tha-ap394:89.4": "gacchāmi mama patthitaṃ.",
"tha-ap394:90.1": "Tena pallaṅkadānena,",
"tha-ap394:90.2": "tamaṃ sabbaṃ vinodayiṃ;",
"tha-ap394:90.3": "Sabbābhiññābalappatto,",
"tha-ap394:90.4": "thero vandati taṃ muni.",
"tha-ap394:91.1": "Parakiccattakiccāni,",
"tha-ap394:91.2": "sabbakiccāni sādhayiṃ;",
"tha-ap394:91.3": "Tena kammena sukatena,",
"tha-ap394:91.4": "pāvisiṃ abhayaṃ puraṃ.",
"tha-ap394:92.1": "Pariniṭṭhitasālamhi,",
"tha-ap394:92.2": "paribhogamadāsahaṃ;",
"tha-ap394:92.3": "Tena kammena sukatena,",
"tha-ap394:92.4": "seṭṭhattaṃ ajjhupāgato.",
"tha-ap394:93.1": "Ye keci damakā loke,",
"tha-ap394:93.2": "hatthiasse damenti ye;",
"tha-ap394:93.3": "Karitvā kāraṇā nānā,",
"tha-ap394:93.4": "dāruṇena damenti te.",
"tha-ap394:94.1": "Na hevaṃ tvaṃ mahāvīra,",
"tha-ap394:94.2": "damesi naranāriyo;",
"tha-ap394:94.3": "Adaṇḍena asatthena,",
"tha-ap394:94.4": "damesi uttame dame.",
"tha-ap394:95.1": "Dānassa vaṇṇe kittento,",
"tha-ap394:95.2": "desanākusalo muni;",
"tha-ap394:95.3": "Ekapañhaṃ kathentova,",
"tha-ap394:95.4": "bodhesi tisate muni.",
"tha-ap394:96.1": "Dantā mayaṃ sārathinā,",
"tha-ap394:96.2": "suvimuttā anāsavā;",
"tha-ap394:96.3": "Sabbābhiññābalapattā,",
"tha-ap394:96.4": "nibbutā upadhikkhaye.",
"tha-ap394:97.1": "Satasahassito kappe,",
"tha-ap394:97.2": "yaṃ dānamadadiṃ tadā;",
"tha-ap394:97.3": "Atikkantā bhayā sabbe,",
"tha-ap394:97.4": "sālādānassidaṃ phalaṃ.",
"tha-ap394:98.1": "Kilesā jhāpitā mayhaṃ, … pe …",
"tha-ap394:98.4": "viharāmi anāsavo.",
"tha-ap394:99.1": "Svāgataṃ vata me āsi, … pe …",
"tha-ap394:99.4": "kataṃ buddhassa sāsanaṃ.",
"tha-ap394:100.1": "Paṭisambhidā catasso, … pe …",
"tha-ap394:100.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap394:101.1": "Itthaṃ sudaṃ āyasmā selo sapariso bhagavato santike imā gāthāyo abhāsitthāti.",
"tha-ap394:102.1": "Selattherassāpadānaṃ dutiyaṃ."
}