{
"tha-ap237:1.1": "Therāpadāna",
"tha-ap237:2.1": "Udakāsanavagga",
"tha-ap237:3.1": "5 Vedikārakattheraapadāna",
"tha-ap237:4.1": "“Vipassino bhagavato,",
"tha-ap237:4.2": "bodhiyā pādaputtame;",
"tha-ap237:4.3": "Pasannacitto sumano,",
"tha-ap237:4.4": "kāresiṃ vedikaṃ ahaṃ.",
"tha-ap237:5.1": "Ekanavutito kappe,",
"tha-ap237:5.2": "kāresiṃ vedikaṃ ahaṃ;",
"tha-ap237:5.3": "Duggatiṃ nābhijānāmi,",
"tha-ap237:5.4": "vedikāya idaṃ phalaṃ.",
"tha-ap237:6.1": "Ito ekādase kappe,",
"tha-ap237:6.2": "ahosiṃ sūriyassamo;",
"tha-ap237:6.3": "Sattaratanasampanno,",
"tha-ap237:6.4": "cakkavattī mahabbalo.",
"tha-ap237:7.1": "Paṭisambhidā catasso, … pe …",
"tha-ap237:7.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap237:8.1": "Itthaṃ sudaṃ āyasmā vedikārako thero imā gāthāyo abhāsitthāti.",
"tha-ap237:9.1": "Vedikārakattherassāpadānaṃ pañcamaṃ."
}