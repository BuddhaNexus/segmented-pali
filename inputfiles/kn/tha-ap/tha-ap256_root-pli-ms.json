{
  "tha-ap256:0.1": "Therāpadāna ",
  "tha-ap256:0.2": "Thomakavagga ",
  "tha-ap256:0.3": "4 Ticampakapupphiyattheraapadāna ",
  "tha-ap256:1.1": "“Himavantassāvidūre, ",
  "tha-ap256:1.2": "vikato nāma pabbato; ",
  "tha-ap256:1.3": "Tassa vemajjhe vasati, ",
  "tha-ap256:1.4": "samaṇo bhāvitindriyo. ",
  "tha-ap256:2.1": "Disvāna tassopasamaṁ, ",
  "tha-ap256:2.2": "vippasannena cetasā; ",
  "tha-ap256:2.3": "Tīṇi campakapupphāni, ",
  "tha-ap256:2.4": "gahetvāna samokiriṁ. ",
  "tha-ap256:3.1": "Ekanavutito kappe, ",
  "tha-ap256:3.2": "yaṁ pupphamabhipūjayiṁ; ",
  "tha-ap256:3.3": "Duggatiṁ nābhijānāmi, ",
  "tha-ap256:3.4": "buddhapūjāyidaṁ phalaṁ. ",
  "tha-ap256:4.1": "Paṭisambhidā catasso, ",
  "tha-ap256:4.2": "…pe… ",
  "tha-ap256:4.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap256:5.1": "Itthaṁ sudaṁ āyasmā ticampakapupphiyo thero imā gāthāyo abhāsitthāti. ",
  "tha-ap256:6.1": "Ticampakapupphiyattherassāpadānaṁ catutthaṁ. "
}