{
  "tha-ap294:0.1": "Therāpadāna ",
  "tha-ap294:0.2": "Citakapūjakavagga ",
  "tha-ap294:0.3": "2. Pupphadhārakattheraapadāna ",
  "tha-ap294:1.1": "“Vākacīradharo āsiṁ, ",
  "tha-ap294:1.2": "ajinuttaravāsano; ",
  "tha-ap294:1.3": "Abhiññā pañca nibbattā, ",
  "tha-ap294:1.4": "candassa parimajjako. ",
  "tha-ap294:2.1": "Vipassiṁ lokapajjotaṁ, ",
  "tha-ap294:2.2": "disvā abhigataṁ mamaṁ; ",
  "tha-ap294:2.3": "Pāricchattakapupphāni, ",
  "tha-ap294:2.4": "dhāresiṁ satthuno ahaṁ. ",
  "tha-ap294:3.1": "Ekanavutito kappe, ",
  "tha-ap294:3.2": "yaṁ pupphamabhipūjayiṁ; ",
  "tha-ap294:3.3": "Duggatiṁ nābhijānāmi, ",
  "tha-ap294:3.4": "dhāraṇāya idaṁ phalaṁ. ",
  "tha-ap294:4.1": "Sattāsītimhito kappe, ",
  "tha-ap294:4.2": "eko āsiṁ mahīpati; ",
  "tha-ap294:4.3": "Samantadhāraṇo nāma, ",
  "tha-ap294:4.4": "cakkavattī mahabbalo. ",
  "tha-ap294:5.1": "Paṭisambhidā catasso, ",
  "tha-ap294:5.2": "…pe… ",
  "tha-ap294:5.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap294:6.1": "Itthaṁ sudaṁ āyasmā pupphadhārako thero imā gāthāyo abhāsitthāti. ",
  "tha-ap294:7.1": "Pupphadhārakattherassāpadānaṁ dutiyaṁ. "
}