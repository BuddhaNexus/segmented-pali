{
"tha-ap98:1.1": "Therāpadāna",
"tha-ap98:2.1": "Sudhāvagga",
"tha-ap98:3.1": "6. Tipupphiyattheraapadāna",
"tha-ap98:4.1": "“Migaluddo pure āsiṃ,",
"tha-ap98:4.2": "araññe kānane ahaṃ;",
"tha-ap98:4.3": "Pāṭaliṃ haritaṃ disvā,",
"tha-ap98:4.4": "tīṇi pupphāni okiriṃ.",
"tha-ap98:5.1": "Patitapattāni gaṇhitvā,",
"tha-ap98:5.2": "bahi chaḍḍesahaṃ tadā;",
"tha-ap98:5.3": "Antosuddhaṃ bahisuddhaṃ,",
"tha-ap98:5.4": "suvimuttaṃ anāsavaṃ.",
"tha-ap98:6.1": "Sammukhā viya sambuddhaṃ,",
"tha-ap98:6.2": "vipassiṃ lokanāyakaṃ;",
"tha-ap98:6.3": "Pāṭaliṃ abhivādetvā,",
"tha-ap98:6.4": "tattha kālaṅkato ahaṃ.",
"tha-ap98:7.1": "Ekanavutito kappe,",
"tha-ap98:7.2": "yaṃ bodhimabhipūjayiṃ;",
"tha-ap98:7.3": "Duggatiṃ nābhijānāmi,",
"tha-ap98:7.4": "bodhipūjāyidaṃ phalaṃ.",
"tha-ap98:8.1": "Samantapāsādikā nāma,",
"tha-ap98:8.2": "terasāsiṃsu rājino;",
"tha-ap98:8.3": "Ito tettiṃsakappamhi,",
"tha-ap98:8.4": "cakkavattī mahabbalā.",
"tha-ap98:9.1": "Paṭisambhidā catasso, … pe …",
"tha-ap98:9.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap98:10.1": "Itthaṃ sudaṃ āyasmā tipupphiyo thero imā gāthāyo abhāsitthāti.",
"tha-ap98:11.1": "Tipupphiyattherassāpadānaṃ chaṭṭhaṃ."
}