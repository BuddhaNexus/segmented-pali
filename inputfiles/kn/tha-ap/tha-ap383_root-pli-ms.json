{
  "tha-ap383:0.1": "Therāpadāna ",
  "tha-ap383:0.2": "Avaṭaphalavagga ",
  "tha-ap383:0.3": "1 Avaṭaphaladāyakattheraapadāna ",
  "tha-ap383:1.1": "“Sataraṁsi nāma bhagavā, ",
  "tha-ap383:1.2": "sayambhū aparājito; ",
  "tha-ap383:1.3": "Vivekakāmo sambuddho, ",
  "tha-ap383:1.4": "gocarāyābhinikkhami. ",
  "tha-ap383:2.1": "Phalahattho ahaṁ disvā, ",
  "tha-ap383:2.2": "upagacchiṁ narāsabhaṁ; ",
  "tha-ap383:2.3": "Pasannacitto sumano, ",
  "tha-ap383:2.4": "avaṭaṁ adadiṁ phalaṁ. ",
  "tha-ap383:3.1": "Catunnavutito kappe, ",
  "tha-ap383:3.2": "yaṁ phalamadadiṁ ahaṁ; ",
  "tha-ap383:3.3": "Duggatiṁ nābhijānāmi, ",
  "tha-ap383:3.4": "phaladānassidaṁ phalaṁ. ",
  "tha-ap383:4.1": "Kilesā jhāpitā mayhaṁ, ",
  "tha-ap383:4.2": "bhavā sabbe samūhatā; ",
  "tha-ap383:4.3": "Nāgova bandhanaṁ chetvā, ",
  "tha-ap383:4.4": "viharāmi anāsavo. ",
  "tha-ap383:5.1": "Svāgataṁ vata me āsi, ",
  "tha-ap383:5.2": "mama buddhassa santike; ",
  "tha-ap383:5.3": "Tisso vijjā anuppattā, ",
  "tha-ap383:5.4": "kataṁ buddhassa sāsanaṁ. ",
  "tha-ap383:6.1": "Paṭisambhidā catasso, ",
  "tha-ap383:6.2": "vimokkhāpi ca aṭṭhime; ",
  "tha-ap383:6.3": "Chaḷabhiññā sacchikatā, ",
  "tha-ap383:6.4": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap383:7.1": "Itthaṁ sudaṁ āyasmā avaṭaphaladāyako thero imā gāthāyo abhāsitthāti. ",
  "tha-ap383:8.1": "Avaṭaphaladāyakattherassāpadānaṁ paṭhamaṁ. "
}