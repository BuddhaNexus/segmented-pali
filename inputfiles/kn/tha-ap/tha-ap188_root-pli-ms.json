{
"tha-ap188:1.1": "Therāpadāna",
"tha-ap188:2.1": "Kuṭajapupphiyavagga",
"tha-ap188:3.1": "6 Bodhiupaṭṭhākattheraapadāna",
"tha-ap188:4.1": "“Nagare rammavatiyā,",
"tha-ap188:4.2": "āsiṃ murajavādako;",
"tha-ap188:4.3": "Niccupaṭṭhānayuttomhi,",
"tha-ap188:4.4": "gatohaṃ bodhimuttamaṃ.",
"tha-ap188:5.1": "Sāyaṃ pātaṃ upaṭṭhitvā,",
"tha-ap188:5.2": "sukkamūlena codito;",
"tha-ap188:5.3": "Aṭṭhārasakappasate,",
"tha-ap188:5.4": "duggatiṃ nupapajjahaṃ.",
"tha-ap188:6.1": "Pannarase kappasate,",
"tha-ap188:6.2": "ito āsiṃ janādhipo;",
"tha-ap188:6.3": "Murajo nāma nāmena,",
"tha-ap188:6.4": "cakkavattī mahabbalo.",
"tha-ap188:7.1": "Paṭisambhidā catasso, … pe …",
"tha-ap188:7.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap188:8.1": "Itthaṃ sudaṃ āyasmā bodhiupaṭṭhāko thero imā gāthāyo abhāsitthāti.",
"tha-ap188:9.1": "Bodhiupaṭṭhākattherassāpadānaṃ chaṭṭhaṃ."
}