{
"tha-ap196:1.1": "Therāpadāna",
"tha-ap196:2.1": "Tamālapupphiyavagga",
"tha-ap196:3.1": "4. Asokapūjakattheraapadāna",
"tha-ap196:4.1": "“Tivarāyaṃ pure ramme,",
"tha-ap196:4.2": "rājuyyānaṃ ahu tadā;",
"tha-ap196:4.3": "Uyyānapālo tatthāsiṃ,",
"tha-ap196:4.4": "rañño baddhacaro ahaṃ.",
"tha-ap196:5.1": "Padumo nāma nāmena,",
"tha-ap196:5.2": "sayambhū sappabho ahu;",
"tha-ap196:5.3": "Nisinnaṃ puṇḍarīkamhi,",
"tha-ap196:5.4": "chāyā na jahi taṃ muniṃ.",
"tha-ap196:6.1": "Asokaṃ pupphitaṃ disvā,",
"tha-ap196:6.2": "piṇḍibhāraṃ sudassanaṃ;",
"tha-ap196:6.3": "Buddhassa abhiropesiṃ,",
"tha-ap196:6.4": "jalajuttamanāmino.",
"tha-ap196:7.1": "Catunnavutito kappe,",
"tha-ap196:7.2": "yaṃ pupphamabhiropayiṃ;",
"tha-ap196:7.3": "Duggatiṃ nābhijānāmi,",
"tha-ap196:7.4": "buddhapūjāyidaṃ phalaṃ.",
"tha-ap196:8.1": "Sattatiṃsamhito kappe,",
"tha-ap196:8.2": "soḷasa araṇañjahā;",
"tha-ap196:8.3": "Sattaratanasampannā,",
"tha-ap196:8.4": "cakkavattī mahabbalā.",
"tha-ap196:9.1": "Paṭisambhidā catasso, … pe …",
"tha-ap196:9.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap196:10.1": "Itthaṃ sudaṃ āyasmā asokapūjako thero imā gāthāyo abhāsitthāti.",
"tha-ap196:11.1": "Asokapūjakattherassāpadānaṃ catutthaṃ."
}