{
"tha-ap91:1.1": "Therāpadāna",
"tha-ap91:2.1": "Timiravagga",
"tha-ap91:3.1": "9. Bodhisiñcakattheraapadāna",
"tha-ap91:4.1": "“Vipassissa bhagavato,",
"tha-ap91:4.2": "mahābodhimaho ahu;",
"tha-ap91:4.3": "Pabbajjupagato santo,",
"tha-ap91:4.4": "upagacchiṃ ahaṃ tadā.",
"tha-ap91:5.1": "Kusumodakamādāya,",
"tha-ap91:5.2": "bodhiyā okiriṃ ahaṃ;",
"tha-ap91:5.3": "Mocayissati no mutto,",
"tha-ap91:5.4": "nibbāpessati nibbuto.",
"tha-ap91:6.1": "Ekanavutito kappe,",
"tha-ap91:6.2": "yaṃ bodhimabhisiñcayiṃ;",
"tha-ap91:6.3": "Duggatiṃ nābhijānāmi,",
"tha-ap91:6.4": "bodhisiñcāyidaṃ phalaṃ.",
"tha-ap91:7.1": "Tettiṃse vattamānamhi,",
"tha-ap91:7.2": "kappe āsuṃ janādhipā;",
"tha-ap91:7.3": "Udakasecanā nāma,",
"tha-ap91:7.4": "aṭṭhete cakkavattino.",
"tha-ap91:8.1": "Paṭisambhidā catasso, … pe …",
"tha-ap91:8.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap91:9.1": "Itthaṃ sudaṃ āyasmā bodhisiñcako thero imā gāthāyo abhāsitthāti.",
"tha-ap91:10.1": "Bodhisiñcakattherassāpadānaṃ navamaṃ."
}