{
"tha-ap443:1.1": "Therāpadāna",
"tha-ap443:2.1": "Vibhītakavagga",
"tha-ap443:3.1": "1 Vibhītakamiñjiyattheraapadāna",
"tha-ap443:4.1": "“Kakusandho mahāvīro,",
"tha-ap443:4.2": "sabbadhammāna pāragū;",
"tha-ap443:4.3": "Gaṇamhā vūpakaṭṭho so,",
"tha-ap443:4.4": "agamāsi vanantaraṃ.",
"tha-ap443:5.1": "Bījamiñjaṃ gahetvāna,",
"tha-ap443:5.2": "latāya āvuṇiṃ ahaṃ;",
"tha-ap443:5.3": "Bhagavā tamhi samaye,",
"tha-ap443:5.4": "jhāyate pabbatantare.",
"tha-ap443:6.1": "Disvānahaṃ devadevaṃ,",
"tha-ap443:6.2": "vippasannena cetasā;",
"tha-ap443:6.3": "Dakkhiṇeyyassa vīrassa,",
"tha-ap443:6.4": "bījamiñjamadāsahaṃ.",
"tha-ap443:7.1": "Imasmiṃyeva kappamhi,",
"tha-ap443:7.2": "yaṃ miñjamadadiṃ tadā;",
"tha-ap443:7.3": "Duggatiṃ nābhijānāmi,",
"tha-ap443:7.4": "bījamiñjassidaṃ phalaṃ.",
"tha-ap443:8.1": "Kilesā jhāpitā mayhaṃ,",
"tha-ap443:8.2": "bhavā sabbe samūhatā;",
"tha-ap443:8.3": "Nāgova bandhanaṃ chetvā,",
"tha-ap443:8.4": "viharāmi anāsavo.",
"tha-ap443:9.1": "Svāgataṃ vata me āsi,",
"tha-ap443:9.2": "mama buddhassa santike;",
"tha-ap443:9.3": "Tisso vijjā anuppattā,",
"tha-ap443:9.4": "kataṃ buddhassa sāsanaṃ.",
"tha-ap443:10.1": "Paṭisambhidā catasso,",
"tha-ap443:10.2": "vimokkhāpi ca aṭṭhime;",
"tha-ap443:10.3": "Chaḷabhiññā sacchikatā,",
"tha-ap443:10.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap443:11.1": "Itthaṃ sudaṃ āyasmā vibhītakamiñjiyo thero imā gāthāyo abhāsitthāti.",
"tha-ap443:12.1": "Vibhītakamiñjiyattherassāpadānaṃ paṭhamaṃ."
}