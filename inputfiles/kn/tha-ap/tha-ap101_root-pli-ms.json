{
"tha-ap101:1.1": "Therāpadāna",
"tha-ap101:2.1": "Sudhāvagga",
"tha-ap101:3.1": "9. Veyyāvaccakattheraapadāna",
"tha-ap101:4.1": "“Vipassissa bhagavato,",
"tha-ap101:4.2": "mahāpūgagaṇo ahu;",
"tha-ap101:4.3": "Veyyāvaccakaro āsiṃ,",
"tha-ap101:4.4": "sabbakiccesu vāvaṭo.",
"tha-ap101:5.1": "Deyyadhammo ca me natthi,",
"tha-ap101:5.2": "sugatassa mahesino;",
"tha-ap101:5.3": "Avandiṃ satthuno pāde,",
"tha-ap101:5.4": "vippasannena cetasā.",
"tha-ap101:6.1": "Ekanavutito kappe,",
"tha-ap101:6.2": "veyyāvaccaṃ akāsahaṃ;",
"tha-ap101:6.3": "Duggatiṃ nābhijānāmi,",
"tha-ap101:6.4": "veyyāvaccassidaṃ phalaṃ.",
"tha-ap101:7.1": "Ito ca aṭṭhame kappe,",
"tha-ap101:7.2": "rājā āsiṃ sucintito;",
"tha-ap101:7.3": "Sattaratanasampanno,",
"tha-ap101:7.4": "cakkavattī mahabbalo.",
"tha-ap101:8.1": "Paṭisambhidā catasso, … pe …",
"tha-ap101:8.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap101:9.1": "Itthaṃ sudaṃ āyasmā veyyāvaccako thero imā gāthāyo abhāsitthāti.",
"tha-ap101:10.1": "Veyyāvaccakattherassāpadānaṃ navamaṃ."
}