{
"tha-ap276:1.1": "Therāpadāna",
"tha-ap276:2.1": "Suvaṇṇabibbohanavagga",
"tha-ap276:3.1": "4. Abbhañjanadāyakattheraapadāna",
"tha-ap276:4.1": "“Koṇḍaññassa bhagavato,",
"tha-ap276:4.2": "vītarāgassa tādino;",
"tha-ap276:4.3": "Ākāsasamacittassa,",
"tha-ap276:4.4": "nippapañcassa jhāyino.",
"tha-ap276:5.1": "Sabbamohātivattassa,",
"tha-ap276:5.2": "sabbalokahitesino;",
"tha-ap276:5.3": "Abbhañjanaṃ mayā dinnaṃ,",
"tha-ap276:5.4": "dvipadindassa tādino.",
"tha-ap276:6.1": "Aparimeyye ito kappe,",
"tha-ap276:6.2": "abbhañjanamadaṃ tadā;",
"tha-ap276:6.3": "Duggatiṃ nābhijānāmi,",
"tha-ap276:6.4": "abbhañjanassidaṃ phalaṃ.",
"tha-ap276:7.1": "Ito pannarase kappe,",
"tha-ap276:7.2": "cirappo nāma khattiyo;",
"tha-ap276:7.3": "Sattaratanasampanno,",
"tha-ap276:7.4": "cakkavattī mahabbalo.",
"tha-ap276:8.1": "Paṭisambhidā catasso, … pe …",
"tha-ap276:8.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap276:9.1": "Itthaṃ sudaṃ āyasmā abbhañjanadāyako thero imā gāthāyo abhāsitthāti.",
"tha-ap276:10.1": "Abbhañjanadāyakattherassāpadānaṃ catutthaṃ."
}