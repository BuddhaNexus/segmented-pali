{
  "tha-ap89:0.1": "Therāpadāna ",
  "tha-ap89:0.2": "Timiravagga ",
  "tha-ap89:0.3": "7. Phaladāyakattheraapadāna ",
  "tha-ap89:1.1": "“Pabbate himavantamhi, ",
  "tha-ap89:1.2": "kharājinadharo ahaṁ; ",
  "tha-ap89:1.3": "Phussaṁ jinavaraṁ disvā, ",
  "tha-ap89:1.4": "phalahattho phalaṁ adaṁ. ",
  "tha-ap89:2.1": "Yamahaṁ phalamadāsiṁ, ",
  "tha-ap89:2.2": "vippasannena cetasā; ",
  "tha-ap89:2.3": "Bhave nibbattamānamhi, ",
  "tha-ap89:2.4": "phalaṁ nibbattate mama. ",
  "tha-ap89:3.1": "Dvenavute ito kappe, ",
  "tha-ap89:3.2": "yaṁ phalaṁ adadiṁ ahaṁ; ",
  "tha-ap89:3.3": "Duggatiṁ nābhijānāmi, ",
  "tha-ap89:3.4": "phaladānassidaṁ phalaṁ. ",
  "tha-ap89:4.1": "Paṭisambhidā catasso, ",
  "tha-ap89:4.2": "…pe… ",
  "tha-ap89:4.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap89:5.1": "Itthaṁ sudaṁ āyasmā phaladāyako thero imā gāthāyo abhāsitthāti. ",
  "tha-ap89:6.1": "Phaladāyakattherassāpadānaṁ sattamaṁ. "
}