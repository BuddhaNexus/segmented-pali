{
"tha-ap114:1.1": "Therāpadāna",
"tha-ap114:2.1": "Mahāparivāravagga",
"tha-ap114:3.1": "2. Sumaṅgalattheraapadāna",
"tha-ap114:4.1": "“Atthadassī jinavaro,",
"tha-ap114:4.2": "lokajeṭṭho narāsabho;",
"tha-ap114:4.3": "Vihārā abhinikkhamma,",
"tha-ap114:4.4": "taḷākaṃ upasaṅkami.",
"tha-ap114:5.1": "Nhatvā pitvā ca sambuddho,",
"tha-ap114:5.2": "uttaritvekacīvaro;",
"tha-ap114:5.3": "Aṭṭhāsi bhagavā tattha,",
"tha-ap114:5.4": "vilokento disodisaṃ.",
"tha-ap114:6.1": "Bhavane upaviṭṭhohaṃ,",
"tha-ap114:6.2": "addasaṃ lokanāyakaṃ;",
"tha-ap114:6.3": "Haṭṭho haṭṭhena cittena,",
"tha-ap114:6.4": "apphoṭesiṃ ahaṃ tadā.",
"tha-ap114:7.1": "Sataraṃsiṃva jotantaṃ,",
"tha-ap114:7.2": "pabhāsantaṃva kañcanaṃ;",
"tha-ap114:7.3": "Naccagīte payuttohaṃ,",
"tha-ap114:7.4": "pañcaṅgatūriyamhi ca.",
"tha-ap114:8.1": "Yaṃ yaṃ yonupapajjāmi,",
"tha-ap114:8.2": "devattaṃ atha mānusaṃ;",
"tha-ap114:8.3": "Sabbe satte abhibhomi,",
"tha-ap114:8.4": "vipulo hoti me yaso.",
"tha-ap114:9.1": "Namo te purisājañña,",
"tha-ap114:9.2": "namo te purisuttama;",
"tha-ap114:9.3": "Attānaṃ tosayitvāna,",
"tha-ap114:9.4": "pare tosesi tvaṃ muni.",
"tha-ap114:10.1": "Pariggahe nisīditvā,",
"tha-ap114:10.2": "hāsaṃ katvāna subbate;",
"tha-ap114:10.3": "Upaṭṭhahitvā sambuddhaṃ,",
"tha-ap114:10.4": "tusitaṃ upapajjahaṃ.",
"tha-ap114:11.1": "Soḷaseto kappasate,",
"tha-ap114:11.2": "dvinavaekacintitā;",
"tha-ap114:11.3": "Sattaratanasampannā,",
"tha-ap114:11.4": "cakkavattī mahabbalā.",
"tha-ap114:12.1": "Paṭisambhidā catasso, … pe …",
"tha-ap114:12.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap114:13.1": "Itthaṃ sudaṃ āyasmā sumaṅgalo thero imā gāthāyo abhāsitthāti.",
"tha-ap114:14.1": "Sumaṅgalattherassāpadānaṃ dutiyaṃ."
}