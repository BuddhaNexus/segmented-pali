{
"kp4:1.1": "Khuddakapāṭha",
"kp4:2.1": "4. Kumārapañhā",
"kp4:3.1": "Ekaṃ nāma kiṃ?",
"kp4:3.2": "Sabbe sattā āhāraṭṭhitikā.",
"kp4:4.1": "Dve nāma kiṃ?",
"kp4:4.2": "Nāmañca rūpañca.",
"kp4:5.1": "Tīṇi nāma kiṃ?",
"kp4:5.2": "Tisso vedanā.",
"kp4:6.1": "Cattāri nāma kiṃ?",
"kp4:6.2": "Cattāri ariyasaccāni.",
"kp4:7.1": "Pañca nāma kiṃ?",
"kp4:7.2": "Pañcupādānakkhandhā.",
"kp4:8.1": "Cha nāma kiṃ?",
"kp4:8.2": "Cha ajjhattikāni āyatanāni.",
"kp4:9.1": "Satta nāma kiṃ?",
"kp4:9.2": "Satta bojjhaṅgā.",
"kp4:10.1": "Aṭṭha nāma kiṃ?",
"kp4:10.2": "Ariyo aṭṭhaṅgiko maggo.",
"kp4:11.1": "Nava nāma kiṃ?",
"kp4:11.2": "Nava sattāvāsā.",
"kp4:12.1": "Dasa nāma kiṃ?",
"kp4:12.2": "Dasahaṅgehi samannāgato “arahā”ti vuccatīti.",
"kp4:13.1": "Kumārapañhā."
}