{
"ne1:1.1": "Netti",
"ne1:2.1": "1. Saṅgahavāra",
"ne1:3.1": "Namo tassa Bhagavato Arahato Sammāsambuddhassa.",
"ne1:4.1": "Yaṃ loko pūjayate,",
"ne1:4.2": "Salokapālo sadā namassati ca;",
"ne1:4.3": "Tasseta sāsanavaraṃ,",
"ne1:4.4": "Vidūhi ñeyyaṃ naravarassa.",
"ne1:5.1": "Dvādasa padāni suttaṃ,",
"ne1:5.2": "Taṃ sabbaṃ byañjanañca attho ca;",
"ne1:5.3": "Taṃ viññeyyaṃ ubhayaṃ,",
"ne1:5.4": "Ko attho byañjanaṃ katamaṃ.",
"ne1:6.1": "Soḷasahārā netti,",
"ne1:6.2": "Pañcanayā sāsanassa pariyeṭṭhi;",
"ne1:6.3": "Aṭṭhārasamūlapadā,",
"ne1:6.4": "Mahakaccānena niddiṭṭhā.",
"ne1:7.1": "Hārā byañjanavicayo,",
"ne1:7.2": "Suttassa nayā tayo ca suttattho;",
"ne1:7.3": "Ubhayaṃ pariggahītaṃ,",
"ne1:7.4": "Vuccati suttaṃ yathāsuttaṃ.",
"ne1:8.1": "Yā ceva desanā yañca,",
"ne1:8.2": "Desitaṃ ubhayameva viññeyyaṃ;",
"ne1:8.3": "Tatrāyamānupubbī,",
"ne1:8.4": "Navavidhasuttantapariyeṭṭhīti.",
"ne1:9.1": "Saṅgahavāro."
}