{
  "anya-e0603n1:0": "Buddhavandanā",
  "anya-e0603n1:1": "1.",
  "anya-e0603n1:2": " Devalokā cavitvāna ,",
  "anya-e0603n1:3": "Mahāmāyāya kucchiyaṃ;",
  "anya-e0603n1:4": "Uppajji guruvāramhi,",
  "anya-e0603n1:5": "Vandetaṃ sakyapuṅgavaṃ.",
  "anya-e0603n1:6": "2.",
  "anya-e0603n1:7": " Dasamā saccaye neso ,",
  "anya-e0603n1:8": "Vijāyi mātukucchito;",
  "anya-e0603n1:9": "Sukkavāre lumbiniyaṃ,",
  "anya-e0603n1:10": "Vande taṃ lokapūjitaṃ.",
  "anya-e0603n1:11": "3.",
  "anya-e0603n1:12": " Cakkavattisiriṃ hitvā,",
  "anya-e0603n1:13": "Mahāsinikkhamaṃ sudhī;",
  "anya-e0603n1:14": "Nikkhamī candavāramhi,",
  "anya-e0603n1:15": "Name taṃ munikuñjaraṃ.",
  "anya-e0603n1:16": "4.",
  "anya-e0603n1:17": " Assatthabodhimūlamhi ,",
  "anya-e0603n1:18": "Pallaṅke aparājite;",
  "anya-e0603n1:19": "Patto sabbaññutaṃ nātho,",
  "anya-e0603n1:20": "Name taṃ budhavāsare.",
  "anya-e0603n1:21": "5.",
  "anya-e0603n1:22": " Pañcavaggiyamukhānaṃ ,",
  "anya-e0603n1:23": "Devānaṃ migadāvane;",
  "anya-e0603n1:24": "Sorivāre dhammacakkaṃ,",
  "anya-e0603n1:25": "Vattesi taṃ jinaṃ name.",
  "anya-e0603n1:26": "6.",
  "anya-e0603n1:27": " Sabbasaṅkhatadhammānaṃ,",
  "anya-e0603n1:28": "Pakāsiyaaniccataṃ;",
  "anya-e0603n1:29": "Nibbutaṃaṅgavāramhi,",
  "anya-e0603n1:30": "Namādhiamatandadaṃ.",
  "anya-e0603n1:31": "7.",
  "anya-e0603n1:32": " Sūriyavāre soṇṇāya ,",
  "anya-e0603n1:33": "Doṇiyā jinaviggaho;",
  "anya-e0603n1:34": "Agginujjalito tassa,",
  "anya-e0603n1:35": "Name dhātusarīrakaṃ."
}