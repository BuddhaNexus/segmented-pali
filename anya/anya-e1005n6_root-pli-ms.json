{
  "anya-e1005n6:0": "7. Pakiṇṇakakaṇḍo",
  "anya-e1005n6:1": "138.",
  "anya-e1005n6:2": " Itthimisse kutosīlaṃ,",
  "anya-e1005n6:3": "Maṃsa bhakkhe kutodayā;",
  "anya-e1005n6:4": "Surā pāne kutosaccaṃ,",
  "anya-e1005n6:5": "Mahālobhe kutohirī;",
  "anya-e1005n6:6": "Mahātande kutosippaṃ,",
  "anya-e1005n6:7": "Mahā kodhe kutodhanaṃ.",
  "anya-e1005n6:8": "139.",
  "anya-e1005n6:9": " Surā yogo vikālo ca,",
  "anya-e1005n6:10": "Samajja caraṇālasaṃ;",
  "anya-e1005n6:11": "Khiḍḍādhutto pāpamitto,",
  "anya-e1005n6:12": "Bhoganāsamukhā ime.",
  "anya-e1005n6:13": "140.",
  "anya-e1005n6:14": " Divā nādikkhā vattabbaṃ,",
  "anya-e1005n6:15": "Ratto nāvacanena ca;",
  "anya-e1005n6:16": "Sañcareyya bhayā bhīto,",
  "anya-e1005n6:17": "Vane vanacarī yathā.",
  "anya-e1005n6:18": "141.",
  "anya-e1005n6:19": " Jīvantāpi matāpañca,",
  "anya-e1005n6:20": "Byāsena parikittitā;",
  "anya-e1005n6:21": "Dukkhito byādhitomūḷho,",
  "anya-e1005n6:22": "Iṇavā nityasevako.",
  "anya-e1005n6:23": "142.",
  "anya-e1005n6:24": " Anāgataṃ bhayaṃ disvā,",
  "anya-e1005n6:25": "Dūrato parivajjaye;",
  "anya-e1005n6:26": "Āgatañca bhayaṃ disvā,",
  "anya-e1005n6:27": "A bhīto hoti paṇḍito.",
  "anya-e1005n6:28": "143.",
  "anya-e1005n6:29": " Niddāluko pamattoca,",
  "anya-e1005n6:30": "Sukhatto rogavālaso;",
  "anya-e1005n6:31": "Mahiccho kammārāmoca,",
  "anya-e1005n6:32": "Satte te satthavajjitā.",
  "anya-e1005n6:33": "144.",
  "anya-e1005n6:34": " Duggataṃ gaccha he lābha,",
  "anya-e1005n6:35": "Lābhī lābhena pūrati;",
  "anya-e1005n6:36": "Thale pavassa pajjunna,",
  "anya-e1005n6:37": "Sindhu āpena pūrati;",
  "anya-e1005n6:38": "Natthidaṃ kammappadhānakaṃ.",
  "anya-e1005n6:39": "145.",
  "anya-e1005n6:40": " Na hi koci kate kicce,",
  "anya-e1005n6:41": "Kattāraṃ samupekkhate;",
  "anya-e1005n6:42": "Tasmā sabbāni kiccāni,",
  "anya-e1005n6:43": "Sāva sesena kāraye.",
  "anya-e1005n6:44": "146.",
  "anya-e1005n6:45": " Tūlaṃ sallahukaṃ loke,",
  "anya-e1005n6:46": "Tato cāpalla-jātiko;",
  "anya-e1005n6:47": "Tato vuḍḍha manovādo,",
  "anya-e1005n6:48": "Pamatto buddhasāsane.",
  "anya-e1005n6:49": "147.",
  "anya-e1005n6:50": " Pāsāṇachattaṃ garukaṃ,",
  "anya-e1005n6:51": "Tato devānacikkhaṇaṃ;",
  "anya-e1005n6:52": "Tato vuḍḍhānamovādo,",
  "anya-e1005n6:53": "Tato buddhassa sāsanaṃ.",
  "anya-e1005n6:54": "148.",
  "anya-e1005n6:55": " Kāyassa dakkhiṇa hattho,",
  "anya-e1005n6:56": "Doso ettha kaniṭṭhako;",
  "anya-e1005n6:57": "Kaṇṇa ghānāna-makkhīnaṃ,",
  "anya-e1005n6:58": "Vāmo tu pāda-pāsako.",
  "anya-e1005n6:59": "149.",
  "anya-e1005n6:60": " Tambūlassa majjha patte,",
  "anya-e1005n6:61": "Kuvero rakkhatī sadā;",
  "anya-e1005n6:62": "Mūlamhi rakkhati yakkho,",
  "anya-e1005n6:63": "Aggamhi kālakaṇṇikā;",
  "anya-e1005n6:64": "Tāni bhuñjeyya chinditvā,",
  "anya-e1005n6:65": "Sirī evaṃ pavaḍḍhati.",
  "anya-e1005n6:66": "150.",
  "anya-e1005n6:67": " Sampuṇṇarakkho brahmāva,",
  "anya-e1005n6:68": "Accurakkho ca bissaṇo;",
  "anya-e1005n6:69": "Tasmā hi te pūjayantu,",
  "anya-e1005n6:70": "Sadā mānenti taṃ naraṃ.",
  "anya-e1005n6:71": "151.",
  "anya-e1005n6:72": " Goṇā hi sabbagihīnaṃ,",
  "anya-e1005n6:73": "Posakā bhogadāyakā;",
  "anya-e1005n6:74": "Tasmā hi mātā pitūva,",
  "anya-e1005n6:75": "Mānaye sakkareyya ca.",
  "anya-e1005n6:76": "152.",
  "anya-e1005n6:77": " Yeca khādanti gomaṃsaṃ,",
  "anya-e1005n6:78": "Mātu maṃsaṃva khādare;",
  "anya-e1005n6:79": "Matesu tesu gijjhānaṃ,",
  "anya-e1005n6:80": "Dade sote ca vāhaye.",
  "anya-e1005n6:81": "153.",
  "anya-e1005n6:82": " Gurusiddho sippārambho,",
  "anya-e1005n6:83": "Ravi sokrā ca majjhimo;",
  "anya-e1005n6:84": "Na sippo buddhacandaro,",
  "anya-e1005n6:85": "Sorī aṅgāca maraṇaṃ.",
  "anya-e1005n6:86": "154.",
  "anya-e1005n6:87": " Aṭṭhamiyaṃ guruṃ hanti,",
  "anya-e1005n6:88": "Sissaṃ hanti catuddasiṃ;",
  "anya-e1005n6:89": "Sippaṃ hanti dasa sippaṃ,",
  "anya-e1005n6:90": "Mātāpitā ca puṇṇamiṃ.",
  "anya-e1005n6:91": "155.",
  "anya-e1005n6:92": " Nāḷikaṃ satta nabhuñje,",
  "anya-e1005n6:93": "Na lābuṃ navamaṃ tathā;",
  "anya-e1005n6:94": "Dvādasa prinnaṃtriminaṃ,",
  "anya-e1005n6:95": "Bhuñje sippaṃ vinassati.",
  "anya-e1005n6:96": "156.",
  "anya-e1005n6:97": " Ekaṃ caje kulaatthaṃ,",
  "anya-e1005n6:98": "Gāmassatthaṃ kuvaṃ caje;",
  "anya-e1005n6:99": "Gāma caje janapadatthaṃ,",
  "anya-e1005n6:100": "Attatthaṃ pathaviṃ caje.",
  "anya-e1005n6:101": "157.",
  "anya-e1005n6:102": " Desaṃ ossajja gacchanti,",
  "anya-e1005n6:103": "Sīho sappuriso gajo;",
  "anya-e1005n6:104": "Tattheva nidhanaṃ yanti,",
  "anya-e1005n6:105": "Kāko kāpuriso migo.",
  "anya-e1005n6:106": "158.",
  "anya-e1005n6:107": " Yamhi padese na māno,",
  "anya-e1005n6:108": "Na pemaṃ na ca bandhavā;",
  "anya-e1005n6:109": "Na ca vijjāgāho koci,",
  "anya-e1005n6:110": "Na tattha vasanaṃ kare.",
  "anya-e1005n6:111": "159.",
  "anya-e1005n6:112": " Caratyekena pādena,",
  "anya-e1005n6:113": "Tiṭṭhatyekena paṇḍito;",
  "anya-e1005n6:114": "A nisamma paraṃ ṭhānaṃ,",
  "anya-e1005n6:115": "Na pubbamālayaṃ jahe.",
  "anya-e1005n6:116": "160.",
  "anya-e1005n6:117": " Dhana dhañña payogesu,",
  "anya-e1005n6:118": "Tathā vijjāgamesu ca;",
  "anya-e1005n6:119": "Dūtesu apacāresu,",
  "anya-e1005n6:120": "Cajjā lajjā tadā bhave.",
  "anya-e1005n6:121": "161.",
  "anya-e1005n6:122": " Dvi guṇo thīnamāhāro,",
  "anya-e1005n6:123": "Buddhicāpi catugguṇo;",
  "anya-e1005n6:124": "Chagguṇo hoti vāyāmo,",
  "anya-e1005n6:125": "Kāmotvaṭṭha-guṇo bhave.",
  "anya-e1005n6:126": "162.",
  "anya-e1005n6:127": " Pabbe pabbe kamenucchu,",
  "anya-e1005n6:128": "Visesarasavāggato;",
  "anya-e1005n6:129": "Tathā sumettiko sādhu,",
  "anya-e1005n6:130": "Viparītova dujjano.",
  "anya-e1005n6:131": "163.",
  "anya-e1005n6:132": " Kassako vāṇijo macco,",
  "anya-e1005n6:133": "Samaṇo suta sīlavā;",
  "anya-e1005n6:134": "Tesu vipula jātesu,",
  "anya-e1005n6:135": "Raṭṭhampi vipulaṃ siyā.",
  "anya-e1005n6:136": "164.",
  "anya-e1005n6:137": " Asajjhāya malā mantā,",
  "anya-e1005n6:138": "Anuṭṭhāna malā gharā;",
  "anya-e1005n6:139": "Malaṃ vaṇṇassa kosajjaṃ,",
  "anya-e1005n6:140": "Pamādo rakkhato malaṃ.",
  "anya-e1005n6:141": "165.",
  "anya-e1005n6:142": " Hīnānaṃ gacchate vittaṃ,",
  "anya-e1005n6:143": "Vīrānaṃ santakattanaṃ;",
  "anya-e1005n6:144": "Vadanti ca hīnā janā,",
  "anya-e1005n6:145": "Pubba-kammappadhānakā.",
  "anya-e1005n6:146": "166.",
  "anya-e1005n6:147": " Na vadanti cevaṃdhīrā,",
  "anya-e1005n6:148": "Vāyamiṃsu sabbakamme;",
  "anya-e1005n6:149": "Na ce sijjhati taṃ kammaṃ,",
  "anya-e1005n6:150": "A-phalaṃ eva ko doso.",
  "anya-e1005n6:151": "167.",
  "anya-e1005n6:152": " Nīcaṃ kulaṃ nipaññaṃ vā,",
  "anya-e1005n6:153": "Nirūpaṃ nibalaṃ samaṃ;",
  "anya-e1005n6:154": "Imaṃ kālaṃ chuttakālaṃ,",
  "anya-e1005n6:155": "Dhanameva visesakaṃ.",
  "anya-e1005n6:156": "Pakiṇṇakakaṇḍo niṭṭhito.",
  "anya-e1005n6:157": "Paṇḍito sujano kaṇḍo,",
  "anya-e1005n6:158": "Dujjano mitta-itthi ca;",
  "anya-e1005n6:159": "Rājā pakiṇṇako cāti,",
  "anya-e1005n6:160": "Satta-kaṇḍa-vibhūsitaṃ.",
  "anya-e1005n6:161": "Visuddhā cāra-therena,",
  "anya-e1005n6:162": "Visuddhārāma-vāsinā;",
  "anya-e1005n6:163": "Sabba-kulānamatthāya,",
  "anya-e1005n6:164": "Visodhitaṃ pathakkhaye."
}