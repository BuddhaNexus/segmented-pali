{
  "anya-e1002n:0": " Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1002n:1": "Nītimañjarī",
  "anya-e1002n:2": "1.",
  "anya-e1002n:3": " Kulakkhaye vinassanti,",
  "anya-e1002n:4": "Kuladhammā sanantanā;",
  "anya-e1002n:5": "Dhamme naṭṭhe kulaṃ sabbaṃ,",
  "anya-e1002n:6": "Adhammo abhibhū khalaṃ.",
  "anya-e1002n:7": "2.",
  "anya-e1002n:8": " Adhammābhibhavā dantā,",
  "anya-e1002n:9": "Padussanti kulitthiyo;",
  "anya-e1002n:10": "Thīsu duṭṭhā sva dhammena,",
  "anya-e1002n:11": "Jāyate vaṇṇasaṅkaro.",
  "anya-e1002n:12": "3.",
  "anya-e1002n:13": " Piyaṃ bhāse guṇaggāho,",
  "anya-e1002n:14": "Sūro siyā vikantano;",
  "anya-e1002n:15": "Dātā candasamā nārī,",
  "anya-e1002n:16": "Diṭṭhaṃ diṭṭhaṃ nahāsaye.",
  "anya-e1002n:17": "4.",
  "anya-e1002n:18": " Kutotthi kumitte saccaṃ,",
  "anya-e1002n:19": "Kudāre rativaḍḍhanaṃ;",
  "anya-e1002n:20": "Kudesamhi mano rammaṃ,",
  "anya-e1002n:21": "Kurāje bhogasampadaṃ.",
  "anya-e1002n:22": "Saṅketeva amittasmiṃ,",
  "anya-e1002n:23": "Mittasmiṃ pi navissase;",
  "anya-e1002n:24": "Abhayā bhaya muppannaṃ,",
  "anya-e1002n:25": "Api mūlāni kantati.",
  "anya-e1002n:26": "Adiṭṭhova paro seyyo,",
  "anya-e1002n:27": "Dummitto no vissāsiko.",
  "anya-e1002n:28": "Aggihomaphalaṃ vedo,",
  "anya-e1002n:29": "Satthaṃsīlaphalaṃ mataṃ;",
  "anya-e1002n:30": "Ratiputtaphalaṃ nārī,",
  "anya-e1002n:31": "Dānabhuttiphalaṃ dhanaṃ.",
  "anya-e1002n:32": "Asaccaṃ sāhasaṃ māyā,",
  "anya-e1002n:33": "Mūḷhatta ma tilobhatā;",
  "anya-e1002n:34": "Asocaṃ niddayattañca,",
  "anya-e1002n:35": "Thīnaṃ dosā sabhāvajā.",
  "anya-e1002n:36": "Jāyāya bhattuno bhāro,",
  "anya-e1002n:37": "Sissena guruno kato;",
  "anya-e1002n:38": "Amaccakehi rājassa,",
  "anya-e1002n:39": "Pitarānaṃ nijenaca.",
  "anya-e1002n:40": "5.",
  "anya-e1002n:41": " Uyyamena hi sijjhanti,",
  "anya-e1002n:42": "Kammāni na manorathā;",
  "anya-e1002n:43": "Na hi suttassa sīhassa,",
  "anya-e1002n:44": "Pavīsanti mukhe migā.",
  "anya-e1002n:45": "Atisītaṃ atiuṇhaṃ,",
  "anya-e1002n:46": "Atisāyamidaṃ ahu;",
  "anya-e1002n:47": "Iti visaṭṭhakammante,",
  "anya-e1002n:48": "Khaṇā accenti māṇave.",
  "anya-e1002n:49": "Ādānassa padānassa,",
  "anya-e1002n:50": "Kattabbassa ca kammuno;",
  "anya-e1002n:51": "Khippaṃ akayyamānassa,",
  "anya-e1002n:52": "Kāle pivati sampadaṃ.",
  "anya-e1002n:53": "Nādabbe nihitā kāci,",
  "anya-e1002n:54": "Kriyā phalavatī bhave;",
  "anya-e1002n:55": "Nabyāpārasatenāpi,",
  "anya-e1002n:56": "Sukova pāṭhate bako.",
  "anya-e1002n:57": "Yo dandhakāle tarati,",
  "anya-e1002n:58": "Taraṇīye ca dandhaye;",
  "anya-e1002n:59": "Sukkhapaṇṇaṃ va akkamma,",
  "anya-e1002n:60": "Atthaṃ bhañjati attano.",
  "anya-e1002n:61": "6.",
  "anya-e1002n:62": " Yaṃ dadāti yaṃ bhuñjati,",
  "anya-e1002n:63": "Tadeva dhanino dhanaṃ;",
  "anya-e1002n:64": "Aññe matassa kīḷanti,",
  "anya-e1002n:65": "Dārehipi dhanehipi.",
  "anya-e1002n:66": "Dānopabhogahīnena ,",
  "anya-e1002n:67": "Dhanena dhanino sukhaṃ;",
  "anya-e1002n:68": "Ko viseso daliddassa,",
  "anya-e1002n:69": "Adhikaṃ dhanarakkhaṇaṃ.",
  "anya-e1002n:70": "Nijasokhyaṃ nirundhanto,",
  "anya-e1002n:71": "Nīcabhogo mitampaco;",
  "anya-e1002n:72": "Dhanaṃ sañcayate yo so,",
  "anya-e1002n:73": "Parabhāravaho pasu.",
  "anya-e1002n:74": "Yaṃ ussukā saṅkharonti,",
  "anya-e1002n:75": "Alakkhikā bahuṃ dhanaṃ;",
  "anya-e1002n:76": "Sippavanto asippāvā,",
  "anya-e1002n:77": "Lakkhi vā tāni bhuñjati.",
  "anya-e1002n:78": "7.",
  "anya-e1002n:79": " Sampatyaṃ mahataṃ cittaṃ,",
  "anya-e1002n:80": "Bhave uppale komalaṃ;",
  "anya-e1002n:81": "Vipatyaṃca mahāsela,",
  "anya-e1002n:82": "Silāsaṅghātakakkasaṃ.",
  "anya-e1002n:83": "8.",
  "anya-e1002n:84": " Asambhabyaguṇaṃ thutvā,",
  "anya-e1002n:85": "Khedo mudhāva jāyate;",
  "anya-e1002n:86": "Avhāyaṃ canda mu llokya,",
  "anya-e1002n:87": "Nacandota mu pāgamī.",
  "anya-e1002n:88": "9.",
  "anya-e1002n:89": " Saccaṃ mukhamhi dhāreyya,",
  "anya-e1002n:90": "Kaṇṇe sutaṃ bhuje jayaṃ;",
  "anya-e1002n:91": "Hadayamhi khamaṃ vīraṃ,",
  "anya-e1002n:92": "Lokādāsaṃca locane.",
  "anya-e1002n:93": "Saddamattaṃ naphandeyya,",
  "anya-e1002n:94": "Aññatvā saddakāraṇaṃ;",
  "anya-e1002n:95": "Saddahetuṃ pariññāya,",
  "anya-e1002n:96": "Pamodo vā bhayo tathā.",
  "anya-e1002n:97": "Sabbasuta ma dhīyeyya,",
  "anya-e1002n:98": "Hīnamukkaṭṭhamajjhimaṃ.",
  "anya-e1002n:99": "10.",
  "anya-e1002n:100": " Dunnāriyā kulaṃ suddhaṃ,",
  "anya-e1002n:101": "Putto nassati lālanā;",
  "anya-e1002n:102": "Samiddhi anayā bandhu,",
  "anya-e1002n:103": "Pavāsā madanā hirī.",
  "anya-e1002n:104": "Lālaye pañcavassāni,",
  "anya-e1002n:105": "Dasavassāni tālaye;",
  "anya-e1002n:106": "Pattetu soḷasevasse,",
  "anya-e1002n:107": "Puttaṃ mittaṃva ācare.",
  "anya-e1002n:108": "Lālane bahavo dosā,",
  "anya-e1002n:109": "Lālane bahavo guṇā.",
  "anya-e1002n:110": "Pāpā nivārayati yojayate hitāya,",
  "anya-e1002n:111": "Guyhāni gūhati guṇaṃ pakaṭīkaroti;",
  "anya-e1002n:112": "Āpattikañca najahāti dadāti kāle,",
  "anya-e1002n:113": "Sammitta lakkhaṇamidaṃ pavadanti santo.",
  "anya-e1002n:114": "11.",
  "anya-e1002n:115": " Dujjano jīyate yutyā,",
  "anya-e1002n:116": "Niggahena nadhīmatā;",
  "anya-e1002n:117": "Nipātyate mahārukkho,",
  "anya-e1002n:118": "Tassamīpa khatikkhayā.",
  "anya-e1002n:119": "Vane migāca luddhānaṃ,",
  "anya-e1002n:120": "Dujjanānañca sajjanā;",
  "anya-e1002n:121": "Akāraṇaverī honti,",
  "anya-e1002n:122": "Tiṇabhakkhā supesalā.",
  "anya-e1002n:123": "Pādalaggaṃ karaṭṭhena,",
  "anya-e1002n:124": "Kaṇḍakeneva kaṇḍakaṃ.",
  "anya-e1002n:125": "Bālaṃ napasse nasuṇe,",
  "anya-e1002n:126": "Nacabālena saṃvase;",
  "anya-e1002n:127": "Bālenāllāpasallāpaṃ,",
  "anya-e1002n:128": "Nakare nacarocaye.",
  "anya-e1002n:129": "12.",
  "anya-e1002n:130": " Upa kattuṃ yathā khuddo,",
  "anya-e1002n:131": "Samattho natathāmahā;",
  "anya-e1002n:132": "Kūpo hi hanti pipāsaṃ,",
  "anya-e1002n:133": "Natu pāyo mahambudhi.",
  "anya-e1002n:134": "13.",
  "anya-e1002n:135": " Ādānassa padānassa,",
  "anya-e1002n:136": "Kattabbassaca kammuno;",
  "anya-e1002n:137": "Khippaṃ akaramānassa,",
  "anya-e1002n:138": "Kālo bhakkhati taṃ rasaṃ.",
  "anya-e1002n:139": "Nakkhattaṃ paṭimānentaṃ,",
  "anya-e1002n:140": "Attho bālaṃ upajjhagā;",
  "anya-e1002n:141": "Attho atthassa nakkhattaṃ,",
  "anya-e1002n:142": "Kiṃ karissanti tārakā.",
  "anya-e1002n:143": "Ajarāmarova pañño,",
  "anya-e1002n:144": "Vijjamatthañca cintaye;",
  "anya-e1002n:145": "Gahitoviya kesesu,",
  "anya-e1002n:146": "Maccunā dhammamācare.",
  "anya-e1002n:147": "14.",
  "anya-e1002n:148": " Vajjā gurūca mantīca,",
  "anya-e1002n:149": "Tayo raṭṭhābhisaṅkhatā;",
  "anya-e1002n:150": "Jīvīta dakkha kosānaṃ,",
  "anya-e1002n:151": "Vaḍḍhanā nāsanāca te.",
  "anya-e1002n:152": "15.",
  "anya-e1002n:153": " Thirena kammaṃ vaḍḍhati,",
  "anya-e1002n:154": "Athirena turena no;",
  "anya-e1002n:155": "Phalanti samaye rukkhā,",
  "anya-e1002n:156": "Sittāpi bahuvārinā.",
  "anya-e1002n:157": "Vāyāmetheva puriso,",
  "anya-e1002n:158": "Nanibbindeyya paṇḍito.",
  "anya-e1002n:159": "Payatano tādiso neva,",
  "anya-e1002n:160": "Kayyo yena phalaṃ nahi;",
  "anya-e1002n:161": "Selagge kūpakhaṇanā,",
  "anya-e1002n:162": "Kathaṃ toyasamāgamo.",
  "anya-e1002n:163": "Ñāṇaṅkusena sammaggaṃ,",
  "anya-e1002n:164": "Niyyatyussāhakuñjaro.",
  "anya-e1002n:165": "Asamekkhitakammantaṃ,",
  "anya-e1002n:166": "Turitābhi nipātinaṃ;",
  "anya-e1002n:167": "Tānikammāni tappenti,",
  "anya-e1002n:168": "Uṇhaṃ va jjhohitaṃ mukhe.",
  "anya-e1002n:169": "16.",
  "anya-e1002n:170": " Chaddosā puriseneha,",
  "anya-e1002n:171": "Hātabbā bhūtimicchantā;",
  "anya-e1002n:172": "Niddā majjaṃ bhayaṃ kodho,",
  "anya-e1002n:173": "Ālasyaṃ dīghasuttatā.",
  "anya-e1002n:174": "Na divā suppasīlena,",
  "anya-e1002n:175": "Rattimuṭṭhānadessinā;",
  "anya-e1002n:176": "Niccasoṇḍena mattena,",
  "anya-e1002n:177": "Sakkā āvasituṃ gharaṃ.",
  "anya-e1002n:178": "Abhetabbamhi bhāyanti,",
  "anya-e1002n:179": "Bhāyitabbe nabhāyare;",
  "anya-e1002n:180": "Bhayābhaya vimuḷhā te,",
  "anya-e1002n:181": "Jimhānugā ujuñjahā.",
  "anya-e1002n:182": "Yassa manussabhūtassa,",
  "anya-e1002n:183": "Natthi bhogāca sippakaṃ;",
  "anya-e1002n:184": "Kiṃ phalaṃ tassa mānussaṃ,",
  "anya-e1002n:185": "Dvipādaṭṭho hi so migo.",
  "anya-e1002n:186": "17.",
  "anya-e1002n:187": " Nānopāyova kattabbo,",
  "anya-e1002n:188": "Sace bhaveyya attano;",
  "anya-e1002n:189": "Atthasiddhi yathākāmaṃ,",
  "anya-e1002n:190": "Upāyo hi hitañjaso.",
  "anya-e1002n:191": "Lañjadānabālisena ,",
  "anya-e1002n:192": "Kūṭaḍḍakāradhīvarā;",
  "anya-e1002n:193": "Vinicchayamahāmacchaṃ,",
  "anya-e1002n:194": "Oṭṭenti lobhasāgare.",
  "anya-e1002n:195": "Yassete caturo dhammā,",
  "anya-e1002n:196": "Vānarinda yathātava;",
  "anya-e1002n:197": "Saccaṃ dhammo dhīti cāgo,",
  "anya-e1002n:198": "Diṭṭhaṃ so ativattati.",
  "anya-e1002n:199": "18.",
  "anya-e1002n:200": " Vidvāca ratanaṃ nārī,",
  "anya-e1002n:201": "Vīṇā sātthaṃ giraṃmahī;",
  "anya-e1002n:202": "Guṇavisesa māgamma,",
  "anya-e1002n:203": "Guṇāni aguṇānica.",
  "anya-e1002n:204": "Dhanavā balavā loke,",
  "anya-e1002n:205": "Dhanā bhavati paṇḍito.",
  "anya-e1002n:206": "Sumane nissito kīṭo,",
  "anya-e1002n:207": "Nigguṇo hīnako sayaṃ;",
  "anya-e1002n:208": "Taṃ pupphehi maṇḍentānaṃ,",
  "anya-e1002n:209": "Raññaṃ siropi rohati.",
  "anya-e1002n:210": "Alakkhikehi sañcītā,",
  "anya-e1002n:211": "Dhanabhogāca cintitā;",
  "anya-e1002n:212": "Lakkhikassa bhavantete,",
  "anya-e1002n:213": "Lakkhivā suṭṭhubhuñjati.",
  "anya-e1002n:214": "Khattiyo seṭṭho jane tasmiṃ,",
  "anya-e1002n:215": "Yo gottapaṭisārino;",
  "anya-e1002n:216": "Vijjācaraṇasampanno,",
  "anya-e1002n:217": "So seṭṭho devamānuse.",
  "anya-e1002n:218": "Visāpi amataṃ gaṇhe,",
  "anya-e1002n:219": "Gūthato maṇimuttamaṃ;",
  "anya-e1002n:220": "Kaṇṭakapādapā pupphaṃ,",
  "anya-e1002n:221": "Thirataṃ dukkulā varaṃ.",
  "anya-e1002n:222": "Dhanissarādiguṇommi -",
  "anya-e1002n:223": "Vegena vāhitā pajā.",
  "anya-e1002n:224": "19.",
  "anya-e1002n:225": " Yassa tthi satataṃ mettā,",
  "anya-e1002n:226": "Sabbalokasuvallabhā;",
  "anya-e1002n:227": "Kūpāyate samuddopi,",
  "anya-e1002n:228": "Aggi tassa jalāyate.",
  "anya-e1002n:229": "20.",
  "anya-e1002n:230": " Sakkharāyati merūpi,",
  "anya-e1002n:231": "Visabhakkho sudhāyate;",
  "anya-e1002n:232": "Sasāyate migarāja,",
  "anya-e1002n:233": "Byālo mālāguṇāyate;",
  "anya-e1002n:234": "Dolāyate chamācālo,",
  "anya-e1002n:235": "Nānāvudhā tiṇāyare.",
  "anya-e1002n:236": "21.",
  "anya-e1002n:237": " Sameva sati ussāhe,",
  "anya-e1002n:238": "Sukhavāho hitaṅkaro;",
  "anya-e1002n:239": "Ūne-dhike tathā nohi,",
  "anya-e1002n:240": "Majjhago sādhu sabbadā.",
  "anya-e1002n:241": "Sādhu kho paṇḍitonāma,",
  "anya-e1002n:242": "Natveva atipaṇḍito."
}