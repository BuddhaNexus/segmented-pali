{
  "anya-e1009n11:0": "Pakiṇṇaka-niddesa",
  "anya-e1009n11:1": "253.",
  "anya-e1009n11:2": " Kumudaṃ ko pabodhayi,",
  "anya-e1009n11:3": "Nātho ravindu paṇḍito;",
  "anya-e1009n11:4": "Kamalaṃ ko kumudaṃ ko,",
  "anya-e1009n11:5": "Narapaṃ ko pabodhayi.",
  "anya-e1009n11:6": "254.",
  "anya-e1009n11:7": " Cittena niyyati loko,",
  "anya-e1009n11:8": "Cittena parikassati;",
  "anya-e1009n11:9": "Cittassa eka-dhammassa,",
  "anya-e1009n11:10": "Sabbeva vasamanvagū.",
  "anya-e1009n11:11": "255.",
  "anya-e1009n11:12": " Samaṇo rājānurājā,",
  "anya-e1009n11:13": "Senāpati mahā-matto;",
  "anya-e1009n11:14": "Dhammaṭṭho paṇḍito disvā,",
  "anya-e1009n11:15": "Paccakkhatthaṃ na kāriyā.",
  "anya-e1009n11:16": "256.",
  "anya-e1009n11:17": " Dīpo nava-disaṃ tejo,",
  "anya-e1009n11:18": "Na heṭṭhā ca tathā sakaṃ;",
  "anya-e1009n11:19": "Para-vajjaṃ vidū passe,",
  "anya-e1009n11:20": "Saka-vajjaṃpi passatu.",
  "anya-e1009n11:21": "257.",
  "anya-e1009n11:22": " Sa-phalaṃ paṇḍito loke,",
  "anya-e1009n11:23": "Sa-kāraṇaṃ vacaṃ bhaṇe;",
  "anya-e1009n11:24": "A-kāraṇaṃphalaṃ bālo,",
  "anya-e1009n11:25": "Idaṃ ubhaya-lakkhaṇaṃ.",
  "anya-e1009n11:26": "258.",
  "anya-e1009n11:27": " Tassa vācāya jāneyya,",
  "anya-e1009n11:28": "Kuṭilaṃ bāla-paṇḍitaṃ;",
  "anya-e1009n11:29": "Vācā-rūpaṃ mittaṃ kare,",
  "anya-e1009n11:30": "Vācā-rūpaṃ dhuvaṃ jahe.",
  "anya-e1009n11:31": "259.",
  "anya-e1009n11:32": " Duccintitassa cintā ca,",
  "anya-e1009n11:33": "Dubbhāsitassa bhāsanā;",
  "anya-e1009n11:34": "Dukkammassa katañcāti,",
  "anya-e1009n11:35": "Etaṃ bālassa lakkhaṇaṃ.",
  "anya-e1009n11:36": "260.",
  "anya-e1009n11:37": " Su-cintitassa cintā ca,",
  "anya-e1009n11:38": "Su-bhāsitassa bhāsanā;",
  "anya-e1009n11:39": "Su-kammassa katañcāti,",
  "anya-e1009n11:40": "Etaṃ dhīrassa lakkhaṇaṃ.",
  "anya-e1009n11:41": "261.",
  "anya-e1009n11:42": " A-nayaṃ nayati dummedho,",
  "anya-e1009n11:43": "A-dhurāyaṃ ni-yuñjati;",
  "anya-e1009n11:44": "Dunnayo seyyaso hoti,",
  "anya-e1009n11:45": "Sammā vutto pakuppati;",
  "anya-e1009n11:46": "Vinayaṃ so na jānāti,",
  "anya-e1009n11:47": "Sādhu tassa a-dassanaṃ.",
  "anya-e1009n11:48": "262.",
  "anya-e1009n11:49": " Nayaṃ nayati medhāvī,",
  "anya-e1009n11:50": "A-dhurāyaṃ na yuñjati;",
  "anya-e1009n11:51": "Su-nayo seyyaso hoti,",
  "anya-e1009n11:52": "Sammā vutto na kuppati;",
  "anya-e1009n11:53": "Vinayaṃ so pajānāti,",
  "anya-e1009n11:54": "Sādhu tena samāgamo.",
  "anya-e1009n11:55": "263.",
  "anya-e1009n11:56": " A-nāyakā vinassanti,",
  "anya-e1009n11:57": "Nassanti bahu-nāyakā;",
  "anya-e1009n11:58": "Thī-nāyakā vinassanti,",
  "anya-e1009n11:59": "Nassanti susu-nāyakā.",
  "anya-e1009n11:60": "264.",
  "anya-e1009n11:61": " Jeṭṭho kammesu nīcānaṃ,",
  "anya-e1009n11:62": "Jānaṃjānaṃva ācare;",
  "anya-e1009n11:63": "A-jānevaṃ kare jānaṃ,",
  "anya-e1009n11:64": "Nīco eti bhayaṃ piyaṃ.",
  "anya-e1009n11:65": "265.",
  "anya-e1009n11:66": " Kammaṃ dujjana-sāruppaṃ,",
  "anya-e1009n11:67": "Dudhā sujana-sāruppaṃ;",
  "anya-e1009n11:68": "Dujjanaṃ tesu dukkamme,",
  "anya-e1009n11:69": "Su-kamme sujanaṃ icche.",
  "anya-e1009n11:70": "266.",
  "anya-e1009n11:71": " Paṇḍito verī bālo ca,",
  "anya-e1009n11:72": "Dujjayo bāla-verito;",
  "anya-e1009n11:73": "Paṇḍitaṃ-verī pamādena,",
  "anya-e1009n11:74": "Na taṃ jayo hi sabbadā.",
  "anya-e1009n11:75": "267.",
  "anya-e1009n11:76": " Guyhassa hi guyhameva sādhu,",
  "anya-e1009n11:77": "Na hi guyhassa pasatthamāvi-kammaṃ;",
  "anya-e1009n11:78": "A-nipphannatāya saheyya dhīro,",
  "anya-e1009n11:79": "Nipphannatthova yathā-sukhaṃ bhaṇeyya.",
  "anya-e1009n11:80": "268.",
  "anya-e1009n11:81": " Guyhamatthaṃ na vivareyya,",
  "anya-e1009n11:82": "Rakkheyya naṃ yathā nidhiṃ;",
  "anya-e1009n11:83": "Na hī pātukato sādhu,",
  "anya-e1009n11:84": "Guyho attho pajānatā.",
  "anya-e1009n11:85": "269.",
  "anya-e1009n11:86": " Thiyā guyhaṃ na saṃseyya,",
  "anya-e1009n11:87": "A-mittassa ca paṇḍito;",
  "anya-e1009n11:88": "Yo cāmisena saṃhīro,",
  "anya-e1009n11:89": "Hadaya-ttheno ca yo naro.",
  "anya-e1009n11:90": "270.",
  "anya-e1009n11:91": " Vivicca bhāseyya divā rahassaṃ,",
  "anya-e1009n11:92": "Rattiṃ giraṃ nāti-velaṃ pamuñce;",
  "anya-e1009n11:93": "Upassutikā hi suṇanti mantaṃ,",
  "anya-e1009n11:94": "Tasmā hi manto khippamupeti bhedaṃ.",
  "anya-e1009n11:95": "271.",
  "anya-e1009n11:96": " Na pakāsati guyhaṃ yo,",
  "anya-e1009n11:97": "So guyhaṃ paṭiguyhati;",
  "anya-e1009n11:98": "Bhayesu na jahe kicce,",
  "anya-e1009n11:99": "Su-mittonucaro bhave.",
  "anya-e1009n11:100": "272.",
  "anya-e1009n11:101": " Karoti dukkaraṃ sādhuṃ,",
  "anya-e1009n11:102": "Ujuṃ khamati dukkhamaṃ;",
  "anya-e1009n11:103": "Duddadaṃ sāmaṃ dadāti,",
  "anya-e1009n11:104": "Yo su-mitto have bhave.",
  "anya-e1009n11:105": "273.",
  "anya-e1009n11:106": " Piya-vācā sadā mitto,",
  "anya-e1009n11:107": "Piya-vatthuṃ na yācanā;",
  "anya-e1009n11:108": "Icchāgatena dānena,",
  "anya-e1009n11:109": "Su-daḷho su-ppiyo have;",
  "anya-e1009n11:110": "Tadaṅgato ca hīnena,",
  "anya-e1009n11:111": "A-ppiyo bhijjano bhave.",
  "anya-e1009n11:112": "274.",
  "anya-e1009n11:113": " Dehīti yācane hirī,",
  "anya-e1009n11:114": "Sirī ca kāya-devatā;",
  "anya-e1009n11:115": "Palāyanti siricchito,",
  "anya-e1009n11:116": "Na yāce para-santakaṃ.",
  "anya-e1009n11:117": "275.",
  "anya-e1009n11:118": " Svāno laddhāna nimmaṃsaṃ,",
  "anya-e1009n11:119": "Aṭṭhiṃ tuṭṭho pamodati;",
  "anya-e1009n11:120": "Sakantikaṃ migaṃ sīho,",
  "anya-e1009n11:121": "Hitvā hatthiṃnudhāvati.",
  "anya-e1009n11:122": "276.",
  "anya-e1009n11:123": " Evaṃ chandānurūpena,",
  "anya-e1009n11:124": "Jano āsīsate phalaṃ;",
  "anya-e1009n11:125": "Mahā chandā mahantaṃva,",
  "anya-e1009n11:126": "Hīnaṃva hīna-chandakā.",
  "anya-e1009n11:127": "277.",
  "anya-e1009n11:128": " Nānā-chandā mahārāja,",
  "anya-e1009n11:129": "Ekāgāre vasāmase;",
  "anya-e1009n11:130": "Ahaṃ gāma-varaṃ icche,",
  "anya-e1009n11:131": "Brāhmaṇī ca gavaṃ sataṃ.",
  "anya-e1009n11:132": "278.",
  "anya-e1009n11:133": " Putto ca ājañña-rathaṃ,",
  "anya-e1009n11:134": "Kaññā ca maṇi-kuṇḍalaṃ;",
  "anya-e1009n11:135": "Yā cesā puṇṇakā jammī,",
  "anya-e1009n11:136": "Ujukkhalaṃbhi-kaṅkhati.",
  "anya-e1009n11:137": "279.",
  "anya-e1009n11:138": " Ṭhānaṃ mitte dhane kamme,",
  "anya-e1009n11:139": "Satussāhe su-labbhitaṃ;",
  "anya-e1009n11:140": "Taṃ daḷhaṃ dukkaraṃ kare,",
  "anya-e1009n11:141": "Paññā-sati-samādhinā.",
  "anya-e1009n11:142": "280.",
  "anya-e1009n11:143": " Bhesajje vihite suddha,",
  "anya-e1009n11:144": "Buddhādi-ratanattaye;",
  "anya-e1009n11:145": "Pasādamācare niccaṃ,",
  "anya-e1009n11:146": "Sajjane sa-guṇepi ca.",
  "anya-e1009n11:147": "281.",
  "anya-e1009n11:148": " Rājā raṭṭhena dhātuso,",
  "anya-e1009n11:149": "Bālo pāpehi dummano,",
  "anya-e1009n11:150": "Alaṅkārena itthīpi,",
  "anya-e1009n11:151": "Kāmehi ca na tappati.",
  "anya-e1009n11:152": "282.",
  "anya-e1009n11:153": " Appiccho ca dhutaṅgena,",
  "anya-e1009n11:154": "Āraddho vīriyena hi;",
  "anya-e1009n11:155": "Visārado parisāya,",
  "anya-e1009n11:156": "Pariccāgena dāyako;",
  "anya-e1009n11:157": "Savanena su-dhammaṃpi,",
  "anya-e1009n11:158": "Na tappativa paṇḍito.",
  "anya-e1009n11:159": "283.",
  "anya-e1009n11:160": " Jeṭṭhassa sitaṃ hasitaṃ,",
  "anya-e1009n11:161": "Majjhassa madhurassaraṃ;",
  "anya-e1009n11:162": "Loke aṃsa-siro-kampaṃ,",
  "anya-e1009n11:163": "Jammassa apa-hassitaṃ;",
  "anya-e1009n11:164": "Etesaṃ ati-hassitaṃ,",
  "anya-e1009n11:165": "Hāso hoti yathākkamaṃ.",
  "anya-e1009n11:166": "284.",
  "anya-e1009n11:167": " Natthi duṭṭhe nayo atthi,",
  "anya-e1009n11:168": "Na dhammo na su-bhāsitaṃ;",
  "anya-e1009n11:169": "Nikkamaṃ duṭṭhe yuñjeyya,",
  "anya-e1009n11:170": "So hi sabbhiṃ na rañjati.",
  "anya-e1009n11:171": "285.",
  "anya-e1009n11:172": " Dullabhaṃ pakatiṃ vācaṃ,",
  "anya-e1009n11:173": "Dullabho khemako suto;",
  "anya-e1009n11:174": "Dullabhā sadisī jāyā,",
  "anya-e1009n11:175": "Dullabho sa-jano piyo.",
  "anya-e1009n11:176": "286.",
  "anya-e1009n11:177": " Dhajo rathassa paññāṇaṃ,",
  "anya-e1009n11:178": "Dhūmo paññāṇamaggino;",
  "anya-e1009n11:179": "Rājā raṭṭhassa paññāṇaṃ,",
  "anya-e1009n11:180": "Bhattā paññāṇamitthiyā.",
  "anya-e1009n11:181": "287.",
  "anya-e1009n11:182": " Dunnāriyā kulaṃ suddhaṃ,",
  "anya-e1009n11:183": "Putto nassati lālanā;",
  "anya-e1009n11:184": "Samiddhi a-nayā bandhu,",
  "anya-e1009n11:185": "Pavāsā madanā hirī.",
  "anya-e1009n11:186": "288.",
  "anya-e1009n11:187": " Mātā pitā ca puttānaṃ,",
  "anya-e1009n11:188": "Novāde bahu-sāsannaṃ;",
  "anya-e1009n11:189": "Paṇḍitā mātaro appaṃ,",
  "anya-e1009n11:190": "Vadeyyuṃ vajja-dīpanaṃ.",
  "anya-e1009n11:191": "289.",
  "anya-e1009n11:192": " Lāḷaye pañcha-vassāni,",
  "anya-e1009n11:193": "Dasa-vassāni tāḷaye;",
  "anya-e1009n11:194": "Patte tu soḷase vasse,",
  "anya-e1009n11:195": "Puttaṃ mittaṃvadācare.",
  "anya-e1009n11:196": "290.",
  "anya-e1009n11:197": " Lālane dhītaraṃ dosā,",
  "anya-e1009n11:198": "Pālane bahavo guṇā;",
  "anya-e1009n11:199": "Dhītuyā kiriyaṃ niccaṃ,",
  "anya-e1009n11:200": "Passantu suṭṭhu mātaro.",
  "anya-e1009n11:201": "Iti pakiṇṇaka-niddeso nāma",
  "anya-e1009n11:202": "Sattamā paricchedo."
}