{
  "anya-e0605n28:0": "Pūjānidhānadīpanīgāthā",
  "anya-e0605n28:1": "189.",
  "anya-e0605n28:2": " Tasamā hi jātovarakamhi tassa,",
  "anya-e0605n28:3": "Āyattake maṅgalacakkavāḷe;",
  "anya-e0605n28:4": "Bhūtehi vatthūhi manoramehi,",
  "anya-e0605n28:5": "Pūjemi taṃ pūjitpūjitaṃ pure.",
  "anya-e0605n28:6": "190.",
  "anya-e0605n28:7": " Sohaṃ ajja panetasmiṃ cakkavāḷamhi pupphite,",
  "anya-e0605n28:8": "Thalaje jalaje vāpi sugandhe ca agandhake.",
  "anya-e0605n28:9": "191.",
  "anya-e0605n28:10": " Manussesu anekattha taḷākuyyānavāpisu,",
  "anya-e0605n28:11": "Pavane himavantasmiṃ tattha satta mahāsare.",
  "anya-e0605n28:12": "192.",
  "anya-e0605n28:13": " Parittadīpe dvisahasse mahādīpe supupphite,",
  "anya-e0605n28:14": "Sattaparibhaṇḍaselesu sinerupabbatuttame.",
  "anya-e0605n28:15": "193.",
  "anya-e0605n28:16": " Kumuduppalakādīni nāgānaṃ bhavanesupi,",
  "anya-e0605n28:17": "Pāṭalādīni pupphāni asurānaṃ hi ālaye.",
  "anya-e0605n28:18": "194.",
  "anya-e0605n28:19": " Koviḷārādikāni tu devatānaṃ hi ālaye,",
  "anya-e0605n28:20": "Evamādī anekattha pupphite dharaṇīruhe.",
  "anya-e0605n28:21": "195.",
  "anya-e0605n28:22": " Campakā salalā nimbā nāgapunnāgaketakā,",
  "anya-e0605n28:23": "Vassikā mallikā sālā koviḷārā ca pāṭali.",
  "anya-e0605n28:24": "196.",
  "anya-e0605n28:25": " Indīvarā asokā ca kaṇikārā ca makulā,",
  "anya-e0605n28:26": "Padumā puṇḍarikā ca sogandhikumuduppalā.",
  "anya-e0605n28:27": "197.",
  "anya-e0605n28:28": " Ete caññe ca rukkhā ca valliyo cāpi pupphitā,",
  "anya-e0605n28:29": "Sugandhā sukhasamphassā nānāvaṇṇanibhā subhā.",
  "anya-e0605n28:30": "198.",
  "anya-e0605n28:31": " Victrā nīlānekāni pītā lohitakāni ca,",
  "anya-e0605n28:32": "Kāḷā setā ca mañjaṭṭha nekavaṇṇā supupphitā.",
  "anya-e0605n28:33": "199.",
  "anya-e0605n28:34": " Sobhate pabbate heṭṭhā sarehi vanarājihi,",
  "anya-e0605n28:35": "Sandamānāhi gaṅgāhi himavā ratanākaro.",
  "anya-e0605n28:36": "200.",
  "anya-e0605n28:37": " Pattakiñjakkhareṇūhi okiṇṇaṃ hoti taṃ vanaṃ,",
  "anya-e0605n28:38": "Bhamarā pupphgandhehi samantā abhināditā.",
  "anya-e0605n28:39": "201.",
  "anya-e0605n28:40": " Athetta sakuṇā santi dijā mañjussarā subhā,",
  "anya-e0605n28:41": "Kūjantamupakūjanti utusampupphite dume.",
  "anya-e0605n28:42": "202.",
  "anya-e0605n28:43": " Niccharānaṃ nipātena pabbatā abhināditā,",
  "anya-e0605n28:44": "Pañcaṅgikāni tūriyāni dibbāni viya suyyare.",
  "anya-e0605n28:45": "203.",
  "anya-e0605n28:46": " Tattha naccanti tasmiṃ jalantaggisikhūpamā,",
  "anya-e0605n28:47": "Tasmiṃ hi kinnarā kiccaṃ padīpena karīyati.",
  "anya-e0605n28:48": "205.",
  "anya-e0605n28:49": " Muttājālāva dissanti niccharānaṃ hi pātakā,",
  "anya-e0605n28:50": "Pajjalantā va tiṭṭhanti maṇiveḷuriyādayo.",
  "anya-e0605n28:51": "206.",
  "anya-e0605n28:52": " Kāḷānusāri taggaraṃ kappūraṃ haricandanaṃ,",
  "anya-e0605n28:53": "Sakuṇānaṃ hi saddena mayūrānaṃ hi kekayā.",
  "anya-e0605n28:54": "207.",
  "anya-e0605n28:55": " Bhamarānaṃ hi ninnādā koñcanādena hatthinaṃ,",
  "anya-e0605n28:56": "Vijambhitena vāḷānaṃ kinnarānaṃ hi gītiyā;",
  "anya-e0605n28:57": "208.",
  "anya-e0605n28:58": " Pabbatānaṃ hi obhāsā maṇīnaṃ jotiyāpi ca,",
  "anya-e0605n28:59": "Vicitrabbhavitānehi dumānaṃ pupphadhūpiyā;",
  "anya-e0605n28:60": "Evaṃ sabbaṅgasampannaṃ kiṃ siyā nandanaṃ vanaṃ.",
  "anya-e0605n28:61": "209.",
  "anya-e0605n28:62": " Evaṃ susamphullavanaṃ hi yaṃ yaṃ,",
  "anya-e0605n28:63": "Tahiṃ tahiṃ pupphitapupphitaṃ subhaṃ;",
  "anya-e0605n28:64": "Mālaṃ susaddañca manuññagandhaṃ,",
  "anya-e0605n28:65": "Pūjemi taṃ pūjitapūjitaṃ purā.",
  "anya-e0605n28:66": "210.",
  "anya-e0605n28:67": " Nāgaloke manusse ca deve brahme ca yaṃ siyā,",
  "anya-e0605n28:68": "Sāmuddikaṃ bhūmigataṃ ākāsaṭṭhañca yaṃ dhamaṃ.",
  "anya-e0605n28:69": "211.",
  "anya-e0605n28:70": " Rajataṃ jātarūpañca muttā veḷuriyā maṇi,",
  "anya-e0605n28:71": "Masāragallaṃ phalikaṃ lohitaṅgaṃ pavāḷakaṃ.",
  "anya-e0605n28:72": "212.",
  "anya-e0605n28:73": " Yo so anantakappesu pūretvā dasapāramī,",
  "anya-e0605n28:74": "Buddho bodhesi sattānaṃ tassa pūjemi taṃ dhanaṃ.",
  "anya-e0605n28:75": "213.",
  "anya-e0605n28:76": " Khomaṃ koseyyaṃ kappāsaṃ sāṇaṃ bhaṅgañca kambalaṃ,",
  "anya-e0605n28:77": "Dukūlāni ca dibbāni dussāni vividhāni te.",
  "anya-e0605n28:78": "214.",
  "anya-e0605n28:79": " Anantavatthadānena hirottappādisaṃvaraṃ,",
  "anya-e0605n28:80": "Yassa siddhaṃ siyā tassa dussāni pujayāmahaṃ.",
  "anya-e0605n28:81": "215.",
  "anya-e0605n28:82": " Pavane jātarukkhānaṃ nānāphalarasuttamaṃ,",
  "anya-e0605n28:83": "Ambā kapiṭṭhā pansā cocamocādinappakā.",
  "anya-e0605n28:84": "216.",
  "anya-e0605n28:85": " Tasmiṃ gandharasaṃ ojaṃ buddhaseṭṭhassa pūjitaṃ,",
  "anya-e0605n28:86": "Vandāmi sirasā niccaṃ vippasannena cetasā.",
  "anya-e0605n28:87": "217.",
  "anya-e0605n28:88": " Pūjemi paṭhamaṃ tassa paṇidhānaṃ acintiyaṃ,",
  "anya-e0605n28:89": "Cakkavāḷamhi sabbehi vijjamānehi vatthuhi.",
  "anya-e0605n28:90": "218.",
  "anya-e0605n28:91": " Dasannaṃ pāramīnantu pūritaṭṭhānamuttamaṃ,",
  "anya-e0605n28:92": "Tato sālavane ramme jātaṭṭhānaṃ carimakaṃ.",
  "anya-e0605n28:93": "219.",
  "anya-e0605n28:94": " Chabbasāni padhānasmiṃ karaṇaṃ dukkarakārikaṃ,",
  "anya-e0605n28:95": "Apparājitapallaṅkaṃ buddhaṃ buddhaguṇaṃ name.",
  "anya-e0605n28:96": "220.",
  "anya-e0605n28:97": " Cuddasa buddhañāṇāni aṭṭharsa āveṇikaṃ,",
  "anya-e0605n28:98": "Pūjemi dasabalañāṇaṃ catuvesārajjamuttamaṃ.",
  "anya-e0605n28:99": "221.",
  "anya-e0605n28:100": " Āsayānusayañāṇaṃ indriyānaṃ paroparaṃ,",
  "anya-e0605n28:101": "Yamakapāṭihīrañca ñāṇaṃ sabbaññutaṃ pi ca.",
  "anya-e0605n28:102": "222.",
  "anya-e0605n28:103": " Mahākaruṇāpattiñāṇaṃ anāvaraṇmiti ca,",
  "anya-e0605n28:104": "Cha asādhāraṇānete ñatvāna pūjayāmahaṃ.",
  "anya-e0605n28:105": "223.",
  "anya-e0605n28:106": " Tato ca sattasattāhe dhammasammasitaṃ name,",
  "anya-e0605n28:107": "Brahmunā yācitaṭṭhānaṃ dhammaṃ desayituṃ varaṃ.",
  "anya-e0605n28:108": "224.",
  "anya-e0605n28:109": " Isipatane migadāye dhammacakkapavattanaṃ,",
  "anya-e0605n28:110": "Tato veḷuvanārāme vasitaṭhānañca pūjaye.",
  "anya-e0605n28:111": "225.",
  "anya-e0605n28:112": " Tato jetavanaṃ rammaṃ ciravutthaṃ mahesinā,",
  "anya-e0605n28:113": "Asādhāraṇamaññesaṃ yamakapāṭihariyaṃ.",
  "anya-e0605n28:114": "226.",
  "anya-e0605n28:115": " Pāricchattakamūlamhi abhidhammañca desanaṃ,",
  "anya-e0605n28:116": "Saṅkassanagaradvāre devorohaṇakaṃ pi ca.",
  "anya-e0605n28:117": "227.",
  "anya-e0605n28:118": " Tato ca himavantasmiṃ mahāsamayadesanaṃ,",
  "anya-e0605n28:119": "Vuttānetāni ṭhānāni natvāna pujayāmahaṃ.",
  "anya-e0605n28:120": "228.",
  "anya-e0605n28:121": " Caturāsītisahassehi dhammakkhandhehi saṅgahaṃ,",
  "anya-e0605n28:122": "Piṭakattayaṃ yathāvuttavidhinā pūjayāmahaṃ.",
  "anya-e0605n28:123": "229.",
  "anya-e0605n28:124": " Mārassa attano āyusaṅkhārosajjanaṃ name,",
  "anya-e0605n28:125": "Kusinārāya mallānaṃ yamakasālamantare.",
  "anya-e0605n28:126": "230.",
  "anya-e0605n28:127": " Paṇidhānamhi paṭṭhāya kataṃ kiccaṃ asesato,",
  "anya-e0605n28:128": "Niṭṭhapetvāna so sabbaṃ parinibbāyināsavo.",
  "anya-e0605n28:129": "231.",
  "anya-e0605n28:130": " Evaṃ nibbāyamānassa katakiccassa tādino,",
  "anya-e0605n28:131": "Ciragatā mahākaruṇā na nibbāyittha kiñcipi.",
  "anya-e0605n28:132": "232.",
  "anya-e0605n28:133": " Svāyaṃ dhammo vinayo ca desito sādhukaṃ mayā,",
  "anya-e0605n28:134": "Mamaccayena so satthā dhātu cāpi sarīrajā.",
  "anya-e0605n28:135": "233.",
  "anya-e0605n28:136": " Apparājitapallaṅkaṃ bodhirukkhañca uttamaṃ,",
  "anya-e0605n28:137": "Mamaccayena satthā ti anujāni mahāmuni.",
  "anya-e0605n28:138": "234.",
  "anya-e0605n28:139": " Mama ṭhane ṭhapetvāna dhātubodhiñca pūjitaṃ,",
  "anya-e0605n28:140": "Anujānāmi tumhākaṃ sādhanatthaṃ sivañjasaṃ.",
  "anya-e0605n28:141": "235.",
  "anya-e0605n28:142": " Tasmā hi tassa saddhammaṃ uggaṇhitvā yathātathaṃ,",
  "anya-e0605n28:143": "Yo deseti sambuddho ti natvāna pūjayāmahaṃ.",
  "anya-e0605n28:144": "236.",
  "anya-e0605n28:145": " Tasmā sāsapamattaṃ pi jinadhātuṃ asesiya,",
  "anya-e0605n28:146": "Vitthinnacakkavāḷamhi natvāna pūjayāmahaṃ.",
  "anya-e0605n28:147": "237.",
  "anya-e0605n28:148": " Paramparābhatānaṃ hi imamhā boddhirukkhato,",
  "anya-e0605n28:149": "Sabbesaṃ bodhirukkhānaṃ natvāna pūjayāmahaṃ.",
  "anya-e0605n28:150": "238.",
  "anya-e0605n28:151": " Yaṃ yaṃ paribhuñji bhagavā pattacīvaramādikaṃ,",
  "anya-e0605n28:152": "Sabbaṃ paribhogadhātuṃ natvāna pūjayāmahaṃ.",
  "anya-e0605n28:153": "239.",
  "anya-e0605n28:154": " Yattha katthaci sayito āsinno caṅkamepi vā,",
  "anya-e0605n28:155": "Pādalañchankaṃ katvā ṭhito natvāna pūjaye.",
  "anya-e0605n28:156": "240.",
  "anya-e0605n28:157": " Na sañjānanti ye buddhaṃ evarūpo ti ñātve,",
  "anya-e0605n28:158": "Kataṃ taṃ paṭimaṃ sabbaṃ natvāna pūjayāmahaṃ.",
  "anya-e0605n28:159": "241.",
  "anya-e0605n28:160": " Evaṃ buddhañca dhammañca saṇghañca anuttaraṃ,",
  "anya-e0605n28:161": "Cakkavāḷamhi sabbehi vatthūhi pūjayāmahaṃ."
}