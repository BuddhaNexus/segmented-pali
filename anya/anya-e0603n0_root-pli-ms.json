{
  "anya-e0603n0:0": " Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e0603n0:1": "Lakkhaṇāto",
  "anya-e0603n0:2": "Buddhathomanāgāthā",
  "anya-e0603n0:3": "1.",
  "anya-e0603n0:4": " Bhavābhavesu nekesu,",
  "anya-e0603n0:5": "Pūresi tiṃsapāramī;",
  "anya-e0603n0:6": "Cariyāyoca sambuddho,",
  "anya-e0603n0:7": "Pariccāgejahaṃsadā.",
  "anya-e0603n0:8": "2.",
  "anya-e0603n0:9": " Vessantarattabhāvamhi ,",
  "anya-e0603n0:10": "Ṭhitomaddiṃ pisoadā;",
  "anya-e0603n0:11": "Nijaṃkaṇhājinaṃjāliṃ,",
  "anya-e0603n0:12": "Viyaṃvaṅkatapabbate.",
  "anya-e0603n0:13": "3.",
  "anya-e0603n0:14": " Tatocavitvātusite,",
  "anya-e0603n0:15": "Pureuppajjipuññavā;",
  "anya-e0603n0:16": "Rūpādidasaṭhānehi ,",
  "anya-e0603n0:17": "Aññedeveatikkami.",
  "anya-e0603n0:18": "4.",
  "anya-e0603n0:19": " Cakkavāḷasahassehi,",
  "anya-e0603n0:20": "Dasahāgammasabbaso;",
  "anya-e0603n0:21": "Yācitodevabrahmūhi,",
  "anya-e0603n0:22": "Buddhabhāvamanonato.",
  "anya-e0603n0:23": "5.",
  "anya-e0603n0:24": " Vilokanānisopañca ,",
  "anya-e0603n0:25": "Viloketvāsirindharo;",
  "anya-e0603n0:26": "Tatocavitvāmāyāya,",
  "anya-e0603n0:27": "Uppajjisākiyekule.",
  "anya-e0603n0:28": "6.",
  "anya-e0603n0:29": " Visākhapuṇṇameseṭṭhe ,",
  "anya-e0603n0:30": "Vijāyilumbinīvane;",
  "anya-e0603n0:31": "Sīhanādaṃpanādento,",
  "anya-e0603n0:32": "Sattapādaṃgatotadā.",
  "anya-e0603n0:33": "8.",
  "anya-e0603n0:34": " Virociṃsutadākāse ,",
  "anya-e0603n0:35": "Tārocandadivākarā;",
  "anya-e0603n0:36": "Ratanānicabhūmaṭṭhā,",
  "anya-e0603n0:37": "Nānāpupphācapupphare.",
  "anya-e0603n0:38": "8.",
  "anya-e0603n0:39": " Sītāyanti avīcaggī ,",
  "anya-e0603n0:40": "Khuppipāsācasohitā;",
  "anya-e0603n0:41": "Lokantaremahāpabhā,",
  "anya-e0603n0:42": "Andhāpassantitāvade.",
  "anya-e0603n0:43": "9.",
  "anya-e0603n0:44": " Aghaṭṭitāvāditāni ,",
  "anya-e0603n0:45": "Tūriyānicaambare;",
  "anya-e0603n0:46": "Madhurenapanādiṃsu,",
  "anya-e0603n0:47": "Yasaṃsarenatāvade.",
  "anya-e0603n0:48": "10.",
  "anya-e0603n0:49": " Thutigītānigāyantā,",
  "anya-e0603n0:50": "Devācakamalāsanā;",
  "anya-e0603n0:51": "Sammodentāvaaṭṭhaṃsu ,",
  "anya-e0603n0:52": "Buddhaṅkurassasantike.",
  "anya-e0603n0:53": "11.",
  "anya-e0603n0:54": " Kumārocārudhātīhi,",
  "anya-e0603n0:55": "Tositārājamandire;",
  "anya-e0603n0:56": "Vuddhosisusukhaṃkhemaṃ,",
  "anya-e0603n0:57": "Sakyakaññānamantare.",
  "anya-e0603n0:58": "12.",
  "anya-e0603n0:59": " Soḷasavassikosakya ,",
  "anya-e0603n0:60": "Nandanopitarāsatā;",
  "anya-e0603n0:61": "Rājatteabhisittosi,",
  "anya-e0603n0:62": "Modesituvanattaye.",
  "anya-e0603n0:63": "13.",
  "anya-e0603n0:64": " Nandanānaṃsovaṇṇa ,",
  "anya-e0603n0:65": "Vaṇṇasobhaṃyasodharaṃ;",
  "anya-e0603n0:66": "Sokāresimahesittaṃ,",
  "anya-e0603n0:67": "Lokanettarasāyanaṃ.",
  "anya-e0603n0:68": "14.",
  "anya-e0603n0:69": " Ekūnatiṃsamevasse,",
  "anya-e0603n0:70": "Visākhapuṇṇamevare;",
  "anya-e0603n0:71": "Sahasenāyauyyāsi,",
  "anya-e0603n0:72": "Yānenuyyānamaṇḍalaṃ.",
  "anya-e0603n0:73": "15.",
  "anya-e0603n0:74": " Nimittecaturodisvā ,",
  "anya-e0603n0:75": "Puttassacavijāyanaṃ;",
  "anya-e0603n0:76": "Sutvāubbiggacittena,",
  "anya-e0603n0:77": "Naramitthapuretahiṃ.",
  "anya-e0603n0:78": "16.",
  "anya-e0603n0:79": " Paccāgammapuraṃtamhā ,",
  "anya-e0603n0:80": "Pattopāsādamaṇḍalaṃ;",
  "anya-e0603n0:81": "Nippajjidevarājāva,",
  "anya-e0603n0:82": "Sayanesomahārahe.",
  "anya-e0603n0:83": "17.",
  "anya-e0603n0:84": " Sundarītaṃpurakkhitvā ,",
  "anya-e0603n0:85": "Naccagītenatosayuṃ;",
  "anya-e0603n0:86": "Evaṃpisonaramitvā,",
  "anya-e0603n0:87": "Āhunikkhamamānaso.",
  "anya-e0603n0:88": "18.",
  "anya-e0603n0:89": " Pathamaṃrāhulaṃputtaṃ,",
  "anya-e0603n0:90": "Passitvānikkhamāmahaṃ;",
  "anya-e0603n0:91": "Iticintiyamāneso,",
  "anya-e0603n0:92": "Agājāyānivesanaṃ;",
  "anya-e0603n0:93": "19.",
  "anya-e0603n0:94": " Thapetvāpādadummāre ,",
  "anya-e0603n0:95": "Gīvamantopavesayaṃ;",
  "anya-e0603n0:96": "Olokayaṃsapassittha,",
  "anya-e0603n0:97": "Nippannaṃtaṃsamātaraṃ.",
  "anya-e0603n0:98": "20.",
  "anya-e0603n0:99": " Gaṇheyyaṃtaṃapanetvā ,",
  "anya-e0603n0:100": "Bāhuṃcedeviyāahaṃ;",
  "anya-e0603n0:101": "Nadadenikkhamokāsaṃ,",
  "anya-e0603n0:102": "Pabujjhitvāyasodharā.",
  "anya-e0603n0:103": "21.",
  "anya-e0603n0:104": " Buddhapattovapasseyyaṃ,",
  "anya-e0603n0:105": "Rāhulaṃiticintiya;",
  "anya-e0603n0:106": "Kaṇḍakaṃāruhitvāna,",
  "anya-e0603n0:107": "Tamhāchannenanikkhami.",
  "anya-e0603n0:108": "22.",
  "anya-e0603n0:109": " Dvārāsayāvivariṃsu ,",
  "anya-e0603n0:110": "Devatādvāramaṇḍalaṃ;",
  "anya-e0603n0:111": "Ratanukkāsahassāni,",
  "anya-e0603n0:112": "Jālayiṃsumarūtahiṃ.",
  "anya-e0603n0:113": "23.",
  "anya-e0603n0:114": " Mānikkhamassusiddhattha,",
  "anya-e0603n0:115": "Itotvaṃsattamedine;",
  "anya-e0603n0:116": "Bhavissasecakkavattī ,",
  "anya-e0603n0:117": "Itinīvāripāpimā.",
  "anya-e0603n0:118": "24.",
  "anya-e0603n0:119": " Tagghajānāmitaṃmāra,",
  "anya-e0603n0:120": "Idhamātiṭṭhapakkama;",
  "anya-e0603n0:121": "Itimāraṃ palāpetvā,",
  "anya-e0603n0:122": "Vissaṭṭhosoabhikkami.",
  "anya-e0603n0:123": "25.",
  "anya-e0603n0:124": " Patvānomanadītīraṃ ,",
  "anya-e0603n0:125": "Punāgamesituragaṃ;",
  "anya-e0603n0:126": "Channañcanagaraṃ bimbā;",
  "anya-e0603n0:127": "Devilāpaṭivedituṃ.",
  "anya-e0603n0:128": "26.",
  "anya-e0603n0:129": " Bandhitaṃyasodharāya,",
  "anya-e0603n0:130": "Moḷiṃsugandhavāsitaṃ;",
  "anya-e0603n0:131": "Chetvākhaggenatiṇhena,",
  "anya-e0603n0:132": "Ukkhipitthatamambare.",
  "anya-e0603n0:133": "27.",
  "anya-e0603n0:134": " Aṭṭhaseṭṭhaparikkhāre ,",
  "anya-e0603n0:135": "Dhāretvābrahmunābhate;",
  "anya-e0603n0:136": "Hitvārājārahaṃdussaṃ,",
  "anya-e0603n0:137": "Samaṇatthamupāgami.",
  "anya-e0603n0:138": "28.",
  "anya-e0603n0:139": " Karontodukkaraṃkammaṃ ,",
  "anya-e0603n0:140": "Chabbassāniparakkamo;",
  "anya-e0603n0:141": "Oḷārikannapānehi;",
  "anya-e0603n0:142": "Dehaṃyāpesiṭhītiyā.",
  "anya-e0603n0:143": "29.",
  "anya-e0603n0:144": " Visākhapuṇṇamehema,",
  "anya-e0603n0:145": "Pātiṃpāyasapūritaṃ;",
  "anya-e0603n0:146": "Sujātāyābhataṃgayha,",
  "anya-e0603n0:147": "Nerañcaramupāgami.",
  "anya-e0603n0:148": "30.",
  "anya-e0603n0:149": " Nerañcarāyatīramhi ,",
  "anya-e0603n0:150": "Sutvāpāyāsamuttamaṃ;",
  "anya-e0603n0:151": "Paṭiyatthavaramaggena,",
  "anya-e0603n0:152": "Bodhimaṇḍamupāgami.",
  "anya-e0603n0:153": "31.",
  "anya-e0603n0:154": " Jayāsanepallaṅkamhi ,",
  "anya-e0603n0:155": "Ṭhitodhiṭṭhesidhītimā;",
  "anya-e0603n0:156": "Navīriyaṃjahissāmi,",
  "anya-e0603n0:157": "Apattoitibuddhataṃ.",
  "anya-e0603n0:158": "32.",
  "anya-e0603n0:159": " Devindodhamayaṃsaṅkhaṃ ,",
  "anya-e0603n0:160": "Setacchattaṃpitāmaho;",
  "anya-e0603n0:161": "Dhārayaṃvādentovīṇaṃ,",
  "anya-e0603n0:162": "Pañcasīkhoṭhitotahiṃ.",
  "anya-e0603n0:163": "33.",
  "anya-e0603n0:164": " Thutigītānigāyantī,",
  "anya-e0603n0:165": "Aṭṭhāsuṃdevasundarī;",
  "anya-e0603n0:166": "Gahetvāhemamañjūsā,",
  "anya-e0603n0:167": "Surapupphehipūritā.",
  "anya-e0603n0:168": "34.",
  "anya-e0603n0:169": " Anatthamentesūriye ,",
  "anya-e0603n0:170": "Mārasenaṃpalāpayī;",
  "anya-e0603n0:171": "Aruṇuggamanekāle,",
  "anya-e0603n0:172": "Pattosibuddhataṃvaraṃ.",
  "anya-e0603n0:173": "35.",
  "anya-e0603n0:174": " Paṭṭhānaṃsammasantassa,",
  "anya-e0603n0:175": "Nāthassadehatosubhā;",
  "anya-e0603n0:176": "Chabbaṇṇabhānicchadhanti ,",
  "anya-e0603n0:177": "Vidhāvanti tahiṃ tahiṃ.",
  "anya-e0603n0:178": "36.",
  "anya-e0603n0:179": " Nīlānīlāpitāpītā,",
  "anya-e0603n0:180": "Rattārattācanicchare;",
  "anya-e0603n0:181": "Setāsetācamañciṭṭhā,",
  "anya-e0603n0:182": "Tamhātamhāpabhassarā.",
  "anya-e0603n0:183": "37.",
  "anya-e0603n0:184": " Buddhapattopibodheyya ,",
  "anya-e0603n0:185": "Dhammagambhīrataṃjino;",
  "anya-e0603n0:186": "Nisāmetvānadesetuṃ,",
  "anya-e0603n0:187": "Nirussāhamupāgato.",
  "anya-e0603n0:188": "38.",
  "anya-e0603n0:189": " Sahampatitadābrahmā ,",
  "anya-e0603n0:190": "Desanāyābhiyācati;",
  "anya-e0603n0:191": "Tenasopathamaṃseṭṭhaṃ,",
  "anya-e0603n0:192": "Dhammacakkamadesayi.",
  "anya-e0603n0:193": "39.",
  "anya-e0603n0:194": " Ekavassovasambuddho ,",
  "anya-e0603n0:195": "Varephagguṇapuṇṇame;",
  "anya-e0603n0:196": "Sakyānaṃnagaraṃgañchī,",
  "anya-e0603n0:197": "Sāvakehipurakkhato.",
  "anya-e0603n0:198": "40.",
  "anya-e0603n0:199": " Samāgamamhiñātīnaṃ,",
  "anya-e0603n0:200": "Vassāpesisusītalaṃ;",
  "anya-e0603n0:201": "Mahāmeghañcadesesi ,",
  "anya-e0603n0:202": "Vessantarassajātakaṃ.",
  "anya-e0603n0:203": "41.",
  "anya-e0603n0:204": " Gantvāsāvakayugena,",
  "anya-e0603n0:205": "Yasodharāyamandiraṃ;",
  "anya-e0603n0:206": "Taṃtosetvānadesesi,",
  "anya-e0603n0:207": "Candakinnarajātakaṃ.",
  "anya-e0603n0:208": "42.",
  "anya-e0603n0:209": " Vasitvāsattamevasse ,",
  "anya-e0603n0:210": "Tāvatiṃse silāsane;",
  "anya-e0603n0:211": "Abhidhammena tosesi,",
  "anya-e0603n0:212": "Devabrahmagaṇaṃbahuṃ.",
  "anya-e0603n0:213": "43.",
  "anya-e0603n0:214": " Pattharaṃñāṇajālena,",
  "anya-e0603n0:215": "Lokaṃlokesisabbadā;",
  "anya-e0603n0:216": "Bodhetuṃbodhiyābhāya;",
  "anya-e0603n0:217": "Bodheyyajanapaṅkajaṃ.",
  "anya-e0603n0:218": "44.",
  "anya-e0603n0:219": " Bodhaneyyaṃjanaṃdisvā ,",
  "anya-e0603n0:220": "Cakkavāḷaparaṃparā;",
  "anya-e0603n0:221": "Gantvāpivibodhesi,",
  "anya-e0603n0:222": "Buddhaseṭṭhosubodhimā.",
  "anya-e0603n0:223": "45.",
  "anya-e0603n0:224": " Bakādikecabrahmāno,",
  "anya-e0603n0:225": "Yakkhecāḷavakādike;",
  "anya-e0603n0:226": "Purindadādikedeve,",
  "anya-e0603n0:227": "Sūcilomādirakkhase.",
  "anya-e0603n0:228": "46.",
  "anya-e0603n0:229": " Jāṇusoṇādikeneke ,",
  "anya-e0603n0:230": "Brāhmaṇecāvikakkhaḷe;",
  "anya-e0603n0:231": "Cūḷodarādikenāge,",
  "anya-e0603n0:232": "Dhanapālādikegaje.",
  "anya-e0603n0:233": "47.",
  "anya-e0603n0:234": " Buddhāmayaṃtimaññante,",
  "anya-e0603n0:235": "Titthiyenekaladdhike;",
  "anya-e0603n0:236": "Damesidamataṃseṭṭho,",
  "anya-e0603n0:237": "Paravādappamaddano.",
  "anya-e0603n0:238": "48.",
  "anya-e0603n0:239": " Anubyañjanasampanno ,",
  "anya-e0603n0:240": "Esabāttiṃsalakkhaṇo;",
  "anya-e0603n0:241": "Byāmappabhāparikkhitto,",
  "anya-e0603n0:242": "Ketumālāsilaṅkhato.",
  "anya-e0603n0:243": "49.",
  "anya-e0603n0:244": " Caturāsītisahassa ,",
  "anya-e0603n0:245": "Dhammakkhandhesadesayi;",
  "anya-e0603n0:246": "Pañcatāthīsavassāni,",
  "anya-e0603n0:247": "Tilokassahitāyave.",
  "anya-e0603n0:248": "50.",
  "anya-e0603n0:249": " Buddhakiccaṃkarontoso,",
  "anya-e0603n0:250": "Vahaṃlokahitaṃvaraṃ;",
  "anya-e0603n0:251": "Mocayaṃbandhanāsatte ,",
  "anya-e0603n0:252": "Kālaṃkhepetisabbadā.",
  "anya-e0603n0:253": "51.",
  "anya-e0603n0:254": " Yahiṃ āsītikonātho,",
  "anya-e0603n0:255": "Nibbutovaramañjake;",
  "anya-e0603n0:256": "Mahāsālavaneramme,",
  "anya-e0603n0:257": "Mallānaṃnigametahiṃ.",
  "anya-e0603n0:258": "52.",
  "anya-e0603n0:259": " Aggikkhandhovalokasmiṃ ,",
  "anya-e0603n0:260": "Jāletvānasasāvako;",
  "anya-e0603n0:261": "Nibbāyitthasaṅkhatānaṃ,",
  "anya-e0603n0:262": "Pakāsentoaniccataṃti.",
  "anya-e0603n0:263": "‘‘Namotebhagavānātha ,",
  "anya-e0603n0:264": "Namotejayataṃjaya;",
  "anya-e0603n0:265": "Namotesirisampanna,",
  "anya-e0603n0:266": "Namotelokanandana.’’"
}