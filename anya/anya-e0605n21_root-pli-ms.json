{
  "anya-e0605n21:0": "Mahāpadhānadīpanīgāthā",
  "anya-e0605n21:1": "111.",
  "anya-e0605n21:2": " Orohitotohitapāpadhammo,",
  "anya-e0605n21:3": "Channena sa channahayena gantvā;",
  "anya-e0605n21:4": "Anomatīramhi anomasatto,",
  "anya-e0605n21:5": "Anomapabbajjamupāgato so.",
  "anya-e0605n21:6": "112.",
  "anya-e0605n21:7": " Nirāmisaṃ pītisukhaṃ anūpamaṃ,",
  "anya-e0605n21:8": "Anūpiye ambavane alattha;",
  "anya-e0605n21:9": "Sarūpasobhāya virūpasobhaṃ,",
  "anya-e0605n21:10": "Sarājikaṃ rājagahaṃ karittha.",
  "anya-e0605n21:11": "113.",
  "anya-e0605n21:12": " Tato aḷāra ūdakatāpasānaṃ,",
  "anya-e0605n21:13": "Jhānenasantuṭṭhamano vihāya;",
  "anya-e0605n21:14": "Mahāpadhānāya uruvelabhūmiṃ,",
  "anya-e0605n21:15": "Gato sikhappattamakāsi dukkaraṃ.",
  "anya-e0605n21:16": "114.",
  "anya-e0605n21:17": " Na kāmato nevatidukkaramhi,",
  "anya-e0605n21:18": "Sabbaññutā sijjhati majjhimāya;",
  "anya-e0605n21:19": "Ñatvāna taṃ pubbaguṇopaladdhaṃ,",
  "anya-e0605n21:20": "Dhammaṃ samānetumagā subodhiṃ."
}