{
  "iti51:0.1_0": "Itivuttaka 51",
  "iti51:0.2_0": "Tikanipāta",
  "iti51:0.3_0": "Paṭhamavagga",
  "iti51:0.4_0": "Dhātusutta",
  "iti51:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti51:2.1_0": "“Tisso imā, bhikkhave, dhātuyo.",
  "iti51:2.2_0": "Katamā tisso?",
  "iti51:2.3_0": "Rūpadhātu, arūpadhātu, nirodhadhātu—",
  "iti51:2.4_0": "imā kho, bhikkhave, tisso dhātuyo”ti.",
  "iti51:2.5_0": "Etamatthaṁ bhagavā avoca.",
  "iti51:2.6_0": "Tatthetaṁ iti vuccati:",
  "iti51:3.1_0": "“Rūpadhātuṁ pariññāya,",
  "iti51:3.2_0": "āruppesu asaṇṭhitā;",
  "iti51:3.3_0": "Nirodhe ye vimuccanti,",
  "iti51:3.4_0": "te janā maccuhāyino.",
  "iti51:4.1_0": "Kāyena amataṁ dhātuṁ,",
  "iti51:4.2_0": "phusayitvā nirūpadhiṁ;",
  "iti51:4.3_0": "Upadhippaṭinissaggaṁ,",
  "iti51:4.4_0": "sacchikatvā anāsavo;",
  "iti51:4.5_0": "Deseti sammāsambuddho,",
  "iti51:4.6_0": "asokaṁ virajaṁ padan”ti.",
  "iti51:5.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti51:5.2_0": "Dutiyaṁ."
}