{
  "tha-ap466:0.1_0": "Therāpadāna",
  "tha-ap466:0.2_0": "Sālakusumiyavagga",
  "tha-ap466:0.3_0": "4. Setudāyakattheraapadāna",
  "tha-ap466:1.1_0": "“Vipassino bhagavato,",
  "tha-ap466:1.2_0": "caṅkamantassa sammukhā;",
  "tha-ap466:1.3_0": "Pasannacitto sumano,",
  "tha-ap466:1.4_0": "setuṁ kārāpayiṁ ahaṁ.",
  "tha-ap466:2.1_0": "Ekanavutito kappe,",
  "tha-ap466:2.2_0": "yaṁ setuṁ kārayiṁ ahaṁ;",
  "tha-ap466:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap466:2.4_0": "setudānassidaṁ phalaṁ.",
  "tha-ap466:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap466:3.2_0": "…pe…",
  "tha-ap466:3.3_0": "viharāmi anāsavo.",
  "tha-ap466:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap466:4.2_0": "…pe…",
  "tha-ap466:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap466:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap466:5.2_0": "…pe…",
  "tha-ap466:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap466:6.1_0": "Itthaṁ sudaṁ āyasmā setudāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap466:7.1_0": "Setudāyakattherassāpadānaṁ catutthaṁ."
}