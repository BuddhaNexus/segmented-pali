{
  "tha-ap451:0.1_0": "Therāpadāna",
  "tha-ap451:0.2_0": "Vibhītakavagga",
  "tha-ap451:0.3_0": "9 Vedikārakattheraapadāna",
  "tha-ap451:1.1_0": "“Padumuttarassa bhagavato,",
  "tha-ap451:1.2_0": "Bodhiyā pādaputtame;",
  "tha-ap451:1.3_0": "Vedikaṁ sukataṁ katvā,",
  "tha-ap451:1.4_0": "Sakaṁ cittaṁ pasādayiṁ.",
  "tha-ap451:2.1_0": "Atoḷārāni bhaṇḍāni,",
  "tha-ap451:2.2_0": "katāni akatāni ca;",
  "tha-ap451:2.3_0": "Antalikkhā pavassanti,",
  "tha-ap451:2.4_0": "vedikāya idaṁ phalaṁ.",
  "tha-ap451:3.1_0": "Ubhato byūḷhasaṅgāme,",
  "tha-ap451:3.2_0": "pakkhandanto bhayānake;",
  "tha-ap451:3.3_0": "Bhayabheravaṁ na passāmi,",
  "tha-ap451:3.4_0": "vedikāya idaṁ phalaṁ.",
  "tha-ap451:4.1_0": "Mama saṅkappamaññāya,",
  "tha-ap451:4.2_0": "byamhaṁ nibbattate subhaṁ;",
  "tha-ap451:4.3_0": "Sayanāni mahagghāni,",
  "tha-ap451:4.4_0": "vedikāya idaṁ phalaṁ.",
  "tha-ap451:5.1_0": "Satasahassito kappe,",
  "tha-ap451:5.2_0": "yaṁ vedikamakārayiṁ;",
  "tha-ap451:5.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap451:5.4_0": "vedikāya idaṁ phalaṁ.",
  "tha-ap451:6.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap451:6.2_0": "…pe…",
  "tha-ap451:6.3_0": "viharāmi anāsavo.",
  "tha-ap451:7.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap451:7.2_0": "…pe…",
  "tha-ap451:7.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap451:8.1_0": "Paṭisambhidā catasso,",
  "tha-ap451:8.2_0": "…pe…",
  "tha-ap451:8.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap451:9.1_0": "Itthaṁ sudaṁ āyasmā vedikārako thero imā gāthāyo abhāsitthāti.",
  "tha-ap451:10.1_0": "Vedikārakattherassāpadānaṁ navamaṁ."
}