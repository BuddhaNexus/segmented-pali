{
  "thi-ap13:0.1_0": "Therīapadāna",
  "thi-ap13:0.2_0": "Ekūposathikavagga",
  "thi-ap13:0.3_0": "3. Modakadāyikātherīapadāna",
  "thi-ap13:1.1_0": "“Nagare bandhumatiyā,",
  "thi-ap13:1.2_0": "kumbhadāsī ahosahaṁ;",
  "thi-ap13:1.3_0": "Mama bhāgaṁ gahetvāna,",
  "thi-ap13:1.4_0": "gacchaṁ udakahārikā.",
  "thi-ap13:2.1_0": "Panthamhi samaṇaṁ disvā,",
  "thi-ap13:2.2_0": "santacittaṁ samāhitaṁ;",
  "thi-ap13:2.3_0": "Pasannacittā sumanā,",
  "thi-ap13:2.4_0": "modake tīṇidāsahaṁ.",
  "thi-ap13:3.1_0": "Tena kammena sukatena,",
  "thi-ap13:3.2_0": "cetanāpaṇidhīhi ca;",
  "thi-ap13:3.3_0": "Ekanavutikappāni,",
  "thi-ap13:3.4_0": "vinipātaṁ nagacchahaṁ.",
  "thi-ap13:4.1_0": "Sampatti taṁ karitvāna,",
  "thi-ap13:4.2_0": "sabbaṁ anubhaviṁ ahaṁ;",
  "thi-ap13:4.3_0": "Modake tīṇi datvāna,",
  "thi-ap13:4.4_0": "pattāhaṁ acalaṁ padaṁ.",
  "thi-ap13:5.1_0": "Kilesā jhāpitā mayhaṁ,",
  "thi-ap13:5.2_0": "…pe…",
  "thi-ap13:5.3_0": "viharāmi anāsavā.",
  "thi-ap13:6.1_0": "Svāgataṁ vata me āsi,",
  "thi-ap13:6.2_0": "…pe…",
  "thi-ap13:6.3_0": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap13:7.1_0": "Paṭisambhidā catasso,",
  "thi-ap13:7.2_0": "…pe…",
  "thi-ap13:7.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "thi-ap13:8.1_0": "Itthaṁ sudaṁ modakadāyikā bhikkhunī imā gāthāyo abhāsitthāti.",
  "thi-ap13:9.1_0": "Modakadāyikātheriyāpadānaṁ tatiyaṁ."
}