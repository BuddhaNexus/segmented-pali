{
  "anya-e0603n2:0_0": "Uṇṇālomikanātha vandanā",
  "anya-e0603n2:1_0": "Uṇṇālodhikanāthassa,",
  "anya-e0603n2:2_0": "Uṇṇāya bhamukantare;",
  "anya-e0603n2:3_0": "Vajirāviya sobhanti,",
  "anya-e0603n2:4_0": "Nikkhanti yo supaṇḍarā.",
  "anya-e0603n2:5_0": "Vajiragghanakāyassa,",
  "anya-e0603n2:6_0": "Nāthassa dehato subhā;",
  "anya-e0603n2:7_0": "Vajireyyā niccharanti;",
  "anya-e0603n2:8_0": "Vijjūva gagaṇantare.",
  "anya-e0603n2:9_0": "Rinindamandahāsena,",
  "anya-e0603n2:10_0": "Dāṭhāya niccharā pabhā;",
  "anya-e0603n2:11_0": "Vipphūrisā disāsabbā,",
  "anya-e0603n2:12_0": "Vajirāyanti paṇḍarā."
}