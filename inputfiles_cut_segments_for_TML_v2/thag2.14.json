{
  "thag2.14:0.1_0": "Theragāthā",
  "thag2.14:0.2_0": "Dukanipāta",
  "thag2.14:0.3_0": "Dutiyavagga",
  "thag2.14:0.4_0": "2.14 Somamittattheragāthā",
  "thag2.14:1.1_0": "“Parittaṁ dārumāruyha,",
  "thag2.14:1.2_0": "yathā sīde mahaṇṇave;",
  "thag2.14:1.3_0": "Evaṁ kusītamāgamma,",
  "thag2.14:1.4_0": "sādhujīvīpi sīdati;",
  "thag2.14:1.5_0": "Tasmā taṁ parivajjeyya,",
  "thag2.14:1.6_0": "kusītaṁ hīnavīriyaṁ.",
  "thag2.14:2.1_0": "Pavivittehi ariyehi,",
  "thag2.14:2.2_0": "pahitattehi jhāyibhi;",
  "thag2.14:2.3_0": "Niccaṁ āraddhavīriyehi,",
  "thag2.14:2.4_0": "paṇḍitehi sahāvase”ti.",
  "thag2.14:2.5_0": "… Somamitto thero …."
}