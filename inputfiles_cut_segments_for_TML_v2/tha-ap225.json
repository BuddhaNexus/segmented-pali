{
  "tha-ap225:0.1_0": "Therāpadāna",
  "tha-ap225:0.2_0": "Ālambaṇadāyakavagga",
  "tha-ap225:0.3_0": "3 Dverataniyattheraapadāna",
  "tha-ap225:1.1_0": "“Migaluddo pure āsiṁ,",
  "tha-ap225:1.2_0": "araññe kānane ahaṁ;",
  "tha-ap225:1.3_0": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap225:1.4_0": "āhutīnaṁ paṭiggahaṁ.",
  "tha-ap225:2.1_0": "Maṁsapesi mayā dinnā,",
  "tha-ap225:2.2_0": "vipassissa mahesino;",
  "tha-ap225:2.3_0": "Sadevakasmiṁ lokasmiṁ,",
  "tha-ap225:2.4_0": "issaraṁ kārayāmahaṁ.",
  "tha-ap225:3.1_0": "Iminā maṁsadānena,",
  "tha-ap225:3.2_0": "ratanaṁ nibbattate mama;",
  "tha-ap225:3.3_0": "Duveme ratanā loke,",
  "tha-ap225:3.4_0": "diṭṭhadhammassa pattiyā.",
  "tha-ap225:4.1_0": "Tehaṁ sabbe anubhomi,",
  "tha-ap225:4.2_0": "maṁsadānassa sattiyā;",
  "tha-ap225:4.3_0": "Gattañca mudukaṁ mayhaṁ,",
  "tha-ap225:4.4_0": "paññā nipuṇavedanī.",
  "tha-ap225:5.1_0": "Ekanavutito kappe,",
  "tha-ap225:5.2_0": "yaṁ maṁsamadadiṁ tadā;",
  "tha-ap225:5.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap225:5.4_0": "maṁsadānassidaṁ phalaṁ.",
  "tha-ap225:6.1_0": "Ito catutthake kappe,",
  "tha-ap225:6.2_0": "eko āsiṁ janādhipo;",
  "tha-ap225:6.3_0": "Mahārohitanāmo so,",
  "tha-ap225:6.4_0": "cakkavattī mahabbalo.",
  "tha-ap225:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap225:7.2_0": "…pe…",
  "tha-ap225:7.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap225:8.1_0": "Itthaṁ sudaṁ āyasmā dverataniyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap225:9.1_0": "Dverataniyattherassāpadānaṁ tatiyaṁ.",
  "tha-ap225:9.2_0": "Dasamaṁ bhāṇavāraṁ."
}