{
  "ja433:0.1_0": "Jātaka",
  "ja433:0.2_0": "Navakanipāta",
  "ja433:0.3_0": "Gijjhavagga",
  "ja433:0.4_0": "7. Lomasakassapajātaka",
  "ja433:1.1_0": "“Assa indasamo rāja,",
  "ja433:1.2_0": "accantaṁ ajarāmaro;",
  "ja433:1.3_0": "Sace tvaṁ yaññaṁ yājeyya,",
  "ja433:1.4_0": "isiṁ lomasakassapaṁ”.",
  "ja433:2.1_0": "“Sasamuddapariyāyaṁ,",
  "ja433:2.2_0": "mahiṁ sāgarakuṇḍalaṁ;",
  "ja433:2.3_0": "Na icche saha nindāya,",
  "ja433:2.4_0": "evaṁ seyya vijānahi.",
  "ja433:3.1_0": "Dhiratthu taṁ yasalābhaṁ,",
  "ja433:3.2_0": "dhanalābhañca brāhmaṇa;",
  "ja433:3.3_0": "Yā vutti vinipātena,",
  "ja433:3.4_0": "adhammacaraṇena vā.",
  "ja433:4.1_0": "Api ce pattamādāya,",
  "ja433:4.2_0": "anagāro paribbaje;",
  "ja433:4.3_0": "Sāyeva jīvikā seyyo,",
  "ja433:4.4_0": "yā cādhammena esanā.",
  "ja433:5.1_0": "Api ce pattamādāya,",
  "ja433:5.2_0": "anagāro paribbaje;",
  "ja433:5.3_0": "Aññaṁ ahiṁsayaṁ loke,",
  "ja433:5.4_0": "api rajjena taṁ varaṁ”.",
  "ja433:6.1_0": "“Balaṁ cando balaṁ suriyo,",
  "ja433:6.2_0": "balaṁ samaṇabrāhmaṇā;",
  "ja433:6.3_0": "Balaṁ velā samuddassa,",
  "ja433:6.4_0": "balātibalamitthiyo.",
  "ja433:7.1_0": "Yathā uggatapaṁ santaṁ,",
  "ja433:7.2_0": "isiṁ lomasakassapaṁ;",
  "ja433:7.3_0": "Pitu atthā candavatī,",
  "ja433:7.4_0": "vājapeyyaṁ ayājayi”.",
  "ja433:8.1_0": "“Taṁ lobhapakataṁ kammaṁ,",
  "ja433:8.2_0": "Kaṭukaṁ kāmahetukaṁ;",
  "ja433:8.3_0": "Tassa mūlaṁ gavesissaṁ,",
  "ja433:8.4_0": "Checchaṁ rāgaṁ sabandhanaṁ.",
  "ja433:9.1_0": "Dhiratthu kāme subahūpi loke,",
  "ja433:9.2_0": "Tapova seyyo kāmaguṇehi rāja;",
  "ja433:9.3_0": "Tapo karissāmi pahāya kāme,",
  "ja433:9.4_0": "Taveva raṭṭhaṁ candavatī ca hotū”ti.",
  "ja433:10.1_0": "Lomasakassapajātakaṁ sattamaṁ."
}