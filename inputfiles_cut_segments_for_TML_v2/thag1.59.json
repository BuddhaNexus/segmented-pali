{
  "thag1.59:0.1_0": "Theragāthā",
  "thag1.59:0.2_0": "Ekakanipāta",
  "thag1.59:0.3_0": "Chaṭṭhavagga",
  "thag1.59:0.4_0": "1.59. Kosalavihārittheragāthā",
  "thag1.59:1.1_0": "“Saddhāyāhaṁ pabbajito,",
  "thag1.59:1.2_0": "araññe me kuṭikā katā;",
  "thag1.59:1.3_0": "Appamatto ca ātāpī,",
  "thag1.59:1.4_0": "sampajāno patissato”ti.",
  "thag1.59:1.5_0": "… Kosalavihāritthero …."
}