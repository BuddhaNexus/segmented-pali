{
  "ja400:0.1_0": "Jātaka",
  "ja400:0.2_0": "Sattakanipāta",
  "ja400:0.3_0": "Kukkuvagga",
  "ja400:0.4_0": "5. Dabbhapupphajātaka",
  "ja400:1.1_0": "“Anutīracārī bhaddante,",
  "ja400:1.2_0": "sahāyamanudhāva maṁ;",
  "ja400:1.3_0": "Mahā me gahito maccho,",
  "ja400:1.4_0": "so maṁ harati vegasā”.",
  "ja400:2.1_0": "“Gambhīracārī bhaddante,",
  "ja400:2.2_0": "daḷhaṁ gaṇhāhi thāmasā;",
  "ja400:2.3_0": "Ahaṁ taṁ uddharissāmi,",
  "ja400:2.4_0": "supaṇṇo uragāmiva”.",
  "ja400:3.1_0": "“Vivādo no samuppanno,",
  "ja400:3.2_0": "dabbhapuppha suṇohi me;",
  "ja400:3.3_0": "Samehi medhagaṁ samma,",
  "ja400:3.4_0": "vivādo vūpasammataṁ”.",
  "ja400:4.1_0": "“Dhammaṭṭhohaṁ pure āsiṁ,",
  "ja400:4.2_0": "bahū aḍḍā me tīritā;",
  "ja400:4.3_0": "Samemi medhagaṁ samma,",
  "ja400:4.4_0": "vivādo vūpasammataṁ.",
  "ja400:5.1_0": "Anutīracāri naṅguṭṭhaṁ,",
  "ja400:5.2_0": "sīsaṁ gambhīracārino;",
  "ja400:5.3_0": "Accāyaṁ majjhimo khaṇḍo,",
  "ja400:5.4_0": "dhammaṭṭhassa bhavissati”.",
  "ja400:6.1_0": "“Cirampi bhakkho abhavissa,",
  "ja400:6.2_0": "sace na vivademase;",
  "ja400:6.3_0": "Asīsakaṁ anaṅguṭṭhaṁ,",
  "ja400:6.4_0": "siṅgālo harati rohitaṁ”.",
  "ja400:7.1_0": "“Yathāpi rājā nandeyya,",
  "ja400:7.2_0": "rajjaṁ laddhāna khattiyo;",
  "ja400:7.3_0": "Evāhamajja nandāmi,",
  "ja400:7.4_0": "disvā puṇṇamukhaṁ patiṁ.",
  "ja400:8.1_0": "Kathaṁ nu thalajo santo,",
  "ja400:8.2_0": "udake macchaṁ parāmasi;",
  "ja400:8.3_0": "Puṭṭho me samma akkhāhi,",
  "ja400:8.4_0": "kathaṁ adhigataṁ tayā”.",
  "ja400:9.1_0": "“Vivādena kisā honti,",
  "ja400:9.2_0": "vivādena dhanakkhayā;",
  "ja400:9.3_0": "Jīnā uddā vivādena,",
  "ja400:9.4_0": "bhuñja māyāvi rohitaṁ.",
  "ja400:10.1_0": "Evameva manussesu,",
  "ja400:10.2_0": "vivādo yattha jāyati;",
  "ja400:10.3_0": "Dhammaṭṭhaṁ paṭidhāvanti,",
  "ja400:10.4_0": "so hi nesaṁ vināyako;",
  "ja400:10.5_0": "Dhanāpi tattha jīyanti,",
  "ja400:10.6_0": "rājakoso pavaḍḍhatī”ti.",
  "ja400:11.1_0": "Dabbhapupphajātakaṁ pañcamaṁ."
}