{
  "ja291:0.1_0": "Jātaka",
  "ja291:0.2_0": "Tikanipāta",
  "ja291:0.3_0": "Kumbhavagga",
  "ja291:0.4_0": "1. Surāghaṭajātaka",
  "ja291:1.1_0": "“Sabbakāmadadaṁ kumbhaṁ,",
  "ja291:1.2_0": "kuṭaṁ laddhāna dhuttako;",
  "ja291:1.3_0": "Yāva naṁ anupāleti,",
  "ja291:1.4_0": "tāva so sukhamedhati.",
  "ja291:2.1_0": "Yadā matto ca ditto ca,",
  "ja291:2.2_0": "pamādā kumbhamabbhidā;",
  "ja291:2.3_0": "Tadā naggo ca pottho ca,",
  "ja291:2.4_0": "pacchā bālo vihaññati.",
  "ja291:3.1_0": "Evameva yo dhanaṁ laddhā,",
  "ja291:3.2_0": "Pamatto paribhuñjati;",
  "ja291:3.3_0": "Pacchā tappati dummedho,",
  "ja291:3.4_0": "Kuṭaṁ bhitvāva dhuttako”ti.",
  "ja291:4.1_0": "Surāghaṭajātakaṁ paṭhamaṁ."
}