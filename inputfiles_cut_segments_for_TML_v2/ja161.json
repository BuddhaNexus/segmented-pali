{
  "ja161:0.1_0": "Jātaka",
  "ja161:0.2_0": "Dukanipāta",
  "ja161:0.3_0": "Santhavavagga",
  "ja161:0.4_0": "1. Indasamānagottajātaka",
  "ja161:1.1_0": "“Na santhavaṁ kāpurisena kayirā,",
  "ja161:1.2_0": "Ariyo anariyena pajānamatthaṁ;",
  "ja161:1.3_0": "Cirānuvutthopi karoti pāpaṁ,",
  "ja161:1.4_0": "Gajo yathā indasamānagottaṁ.",
  "ja161:2.1_0": "Yaṁ tveva jaññā sadiso mamanti,",
  "ja161:2.2_0": "Sīlena paññāya sutena cāpi;",
  "ja161:2.3_0": "Teneva mettiṁ kayirātha saddhiṁ,",
  "ja161:2.4_0": "Sukho have sappurisena saṅgamo”ti.",
  "ja161:3.1_0": "Indasamānagottajātakaṁ paṭhamaṁ."
}