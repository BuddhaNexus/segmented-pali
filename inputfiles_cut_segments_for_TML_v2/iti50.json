{
  "iti50:0.1_0": "Itivuttaka 50",
  "iti50:0.2_0": "Tikanipāta",
  "iti50:0.3_0": "Paṭhamavagga",
  "iti50:0.4_0": "Mūlasutta",
  "iti50:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti50:2.1_0": "“Tīṇimāni, bhikkhave, akusalamūlāni.",
  "iti50:2.2_0": "Katamāni tīṇi?",
  "iti50:2.3_0": "Lobho akusalamūlaṁ, doso akusalamūlaṁ, moho akusalamūlaṁ—",
  "iti50:2.4_0": "imāni kho, bhikkhave, tīṇi akusalamūlānī”ti.",
  "iti50:2.5_0": "Etamatthaṁ bhagavā avoca.",
  "iti50:2.6_0": "Tatthetaṁ iti vuccati:",
  "iti50:3.1_0": "“Lobho doso ca moho ca,",
  "iti50:3.2_0": "purisaṁ pāpacetasaṁ;",
  "iti50:3.3_0": "Hiṁsanti attasambhūtā,",
  "iti50:3.4_0": "tacasāraṁva samphalan”ti.",
  "iti50:4.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti50:4.2_0": "Paṭhamaṁ."
}