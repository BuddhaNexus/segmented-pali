{
  "tha-ap143:0.1_0": "Therāpadāna",
  "tha-ap143:0.2_0": "Chattavagga",
  "tha-ap143:0.3_0": "1 Atichattiyattheraapadāna",
  "tha-ap143:1.1_0": "“Parinibbute bhagavati,",
  "tha-ap143:1.2_0": "atthadassīnaruttame;",
  "tha-ap143:1.3_0": "Chattātichattaṁ kāretvā,",
  "tha-ap143:1.4_0": "thūpamhi abhiropayiṁ.",
  "tha-ap143:2.1_0": "Kālena kālamāgantvā,",
  "tha-ap143:2.2_0": "namassiṁ lokanāyakaṁ;",
  "tha-ap143:2.3_0": "Pupphacchadanaṁ katvāna,",
  "tha-ap143:2.4_0": "chattamhi abhiropayiṁ.",
  "tha-ap143:3.1_0": "Sattarase kappasate,",
  "tha-ap143:3.2_0": "devarajjamakārayiṁ;",
  "tha-ap143:3.3_0": "Manussattaṁ na gacchāmi,",
  "tha-ap143:3.4_0": "thūpapūjāyidaṁ phalaṁ.",
  "tha-ap143:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap143:4.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap143:4.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap143:4.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap143:5.1_0": "Itthaṁ sudaṁ āyasmā atichattiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap143:6.1_0": "Atichattiyattherassāpadānaṁ paṭhamaṁ."
}