{
  "vv62:0.1_0": "Vimānavatthu",
  "vv62:0.2_0": "Purisavimāna",
  "vv62:0.3_0": "Mahārathavagga",
  "vv62:0.4_0": "12. Tatiyanāgavimānavatthu",
  "vv62:1.1_0": "“Ko nu dibbena yānena,",
  "vv62:1.2_0": "sabbasetena hatthinā;",
  "vv62:1.3_0": "Tūriyatāḷitanigghoso,",
  "vv62:1.4_0": "antalikkhe mahīyati.",
  "vv62:2.1_0": "Devatā nusi gandhabbo,",
  "vv62:2.2_0": "adu sakko purindado;",
  "vv62:2.3_0": "Ajānantā taṁ pucchāma,",
  "vv62:2.4_0": "kathaṁ jānemu taṁ mayan”ti.",
  "vv62:3.1_0": "“Namhi devo na gandhabbo,",
  "vv62:3.2_0": "nāpi sakko purindado;",
  "vv62:3.3_0": "Sudhammā nāma ye devā,",
  "vv62:3.4_0": "tesaṁ aññataro ahan”ti.",
  "vv62:4.1_0": "“Pucchāma devaṁ sudhammaṁ,",
  "vv62:4.2_0": "Puthuṁ katvāna añjaliṁ;",
  "vv62:4.3_0": "Kiṁ katvā mānuse kammaṁ,",
  "vv62:4.4_0": "Sudhammaṁ upapajjatī”ti.",
  "vv62:5.1_0": "“Ucchāgāraṁ tiṇāgāraṁ,",
  "vv62:5.2_0": "vatthāgārañca yo dade;",
  "vv62:5.3_0": "Tiṇṇaṁ aññataraṁ datvā,",
  "vv62:5.4_0": "sudhammaṁ upapajjatī”ti.",
  "vv62:6.1_0": "Tatiyanāgavimānaṁ dvādasamaṁ."
}