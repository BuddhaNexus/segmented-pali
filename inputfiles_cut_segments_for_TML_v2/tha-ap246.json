{
  "tha-ap246:0.1_0": "Therāpadāna",
  "tha-ap246:0.2_0": "Tuvaradāyakavagga",
  "tha-ap246:0.3_0": "4 Viravapupphiyattheraapadāna",
  "tha-ap246:1.1_0": "“Khīṇāsavasahassehi,",
  "tha-ap246:1.2_0": "niyyāti lokanāyako;",
  "tha-ap246:1.3_0": "Viravapupphamādāya,",
  "tha-ap246:1.4_0": "buddhassa abhiropayiṁ.",
  "tha-ap246:2.1_0": "Ekanavutito kappe,",
  "tha-ap246:2.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap246:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap246:2.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap246:3.1_0": "Paṭisambhidā catasso,",
  "tha-ap246:3.2_0": "…pe…",
  "tha-ap246:3.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap246:4.1_0": "Itthaṁ sudaṁ āyasmā viravapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap246:5.1_0": "Viravapupphiyattherassāpadānaṁ catutthaṁ."
}