{
  "tha-ap511:0.1_0": "Therāpadāna",
  "tha-ap511:0.2_0": "Kaṇikāravagga",
  "tha-ap511:0.3_0": "9 Tālaphaladāyakattheraapadāna",
  "tha-ap511:1.1_0": "“Sataraṁsī nāma bhagavā,",
  "tha-ap511:1.2_0": "sayambhū aparājito;",
  "tha-ap511:1.3_0": "Vivekā vuṭṭhahitvāna,",
  "tha-ap511:1.4_0": "gocarāyābhinikkhami.",
  "tha-ap511:2.1_0": "Phalahattho ahaṁ disvā,",
  "tha-ap511:2.2_0": "upagacchiṁ narāsabhaṁ;",
  "tha-ap511:2.3_0": "Pasannacitto sumano,",
  "tha-ap511:2.4_0": "tālaphalaṁ adāsahaṁ.",
  "tha-ap511:3.1_0": "Catunnavutito kappe,",
  "tha-ap511:3.2_0": "yaṁ phalaṁ adadiṁ tadā;",
  "tha-ap511:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap511:3.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap511:4.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap511:4.2_0": "…pe…",
  "tha-ap511:4.3_0": "viharāmi anāsavo.",
  "tha-ap511:5.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap511:5.2_0": "…pe…",
  "tha-ap511:5.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap511:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap511:6.2_0": "…pe…",
  "tha-ap511:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap511:7.1_0": "Itthaṁ sudaṁ āyasmā tālaphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap511:8.1_0": "Tālaphaladāyakattherassāpadānaṁ navamaṁ."
}