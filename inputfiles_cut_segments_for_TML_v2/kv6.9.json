{
  "kv6.9:0.1_0": "Kathāvatthu",
  "kv6.9:0.2_0": "Mahāpaṇṇāsaka",
  "kv6.9:0.3_0": "Chaṭṭhavagga",
  "kv6.9:0.4_0": "Cakkhundriyaṁsanidassanantiādikathā",
  "kv6.9:1.1_0": "Cakkhundriyaṁ sanidassananti?",
  "kv6.9:1.2_0": "Āmantā.",
  "kv6.9:1.3_0": "Rūpaṁ rūpāyatanaṁ rūpadhātu …pe…",
  "kv6.9:1.4_0": "cakkhussa āpāthaṁ āgacchatīti?",
  "kv6.9:1.5_0": "Na hevaṁ vattabbe …pe….",
  "kv6.9:2.1_0": "Cakkhundriyaṁ sanidassananti?",
  "kv6.9:2.2_0": "Āmantā.",
  "kv6.9:2.3_0": "Cakkhuñca paṭicca cakkhundriyañca uppajjati cakkhuviññāṇanti?",
  "kv6.9:2.4_0": "Na hevaṁ vattabbe …pe….",
  "kv6.9:3.1_0": "Cakkhuñca paṭicca cakkhundriyañca uppajjati cakkhuviññāṇanti?",
  "kv6.9:3.2_0": "Āmantā.",
  "kv6.9:3.3_0": "“Cakkhuñca paṭicca cakkhundriyañca uppajjati cakkhuviññāṇan”ti—",
  "kv6.9:3.4_0": "attheva suttantoti?",
  "kv6.9:3.5_0": "Natthi.",
  "kv6.9:3.6_0": "“Cakkhuñca paṭicca rūpe ca uppajjati cakkhuviññāṇan”ti—",
  "kv6.9:3.7_0": "attheva suttantoti?",
  "kv6.9:3.8_0": "Āmantā.",
  "kv6.9:3.9_0": "Hañci “cakkhuñca paṭicca rūpe ca uppajjati cakkhuviññāṇan”ti—",
  "kv6.9:3.10_0": "attheva suttanto, no ca vata re vattabbe—",
  "kv6.9:3.11_0": "“cakkhuñca paṭicca cakkhundriyañca uppajjati cakkhuviññāṇan”ti.",
  "kv6.9:4.1_0": "Na vattabbaṁ—",
  "kv6.9:4.2_0": "“pañcindriyāni sanidassanānī”ti?",
  "kv6.9:4.3_0": "Āmantā.",
  "kv6.9:4.4_0": "Nanu passati cakkhuṁ sotaṁ ghānaṁ jivhaṁ kāyanti?",
  "kv6.9:4.5_0": "Āmantā.",
  "kv6.9:4.6_0": "Hañci passati cakkhuṁ sotaṁ ghānaṁ jivhaṁ kāyaṁ, tena vata re vattabbe—",
  "kv6.9:4.7_0": "“pañcindriyāni sanidassanānī”ti …pe….",
  "kv6.9:4.8_0": "Cakkhundriyaṁ sanidassanantiādikathā niṭṭhitā."
}