{
  "thag1.118:0.1_0": "Theragāthā",
  "thag1.118:0.2_0": "Ekakanipāta",
  "thag1.118:0.3_0": "Dvādasamavagga",
  "thag1.118:0.4_0": "1.118 Kimilattheragāthā",
  "thag1.118:1.1_0": "“Abhisattova nipatati,",
  "thag1.118:1.2_0": "Vayo rūpaṁ aññamiva tatheva santaṁ;",
  "thag1.118:1.3_0": "Tasseva sato avippavasato,",
  "thag1.118:1.4_0": "Aññasseva sarāmi attānan”ti.",
  "thag1.118:1.5_0": "… Kimilo thero …."
}