{
  "vv64:0.1_0": "Vimānavatthu",
  "vv64:0.2_0": "Purisavimāna",
  "vv64:0.3_0": "Mahārathavagga",
  "vv64:0.4_0": "14. Mahārathavimānavatthu",
  "vv64:1.1_0": "“Sahassayuttaṁ hayavāhanaṁ subhaṁ,",
  "vv64:1.2_0": "Āruyhimaṁ sandanaṁ nekacittaṁ;",
  "vv64:1.3_0": "Uyyānabhūmiṁ abhito anukkamaṁ,",
  "vv64:1.4_0": "Purindado bhūtapatīva vāsavo.",
  "vv64:2.1_0": "Sovaṇṇamayā te rathakubbarā ubho,",
  "vv64:2.2_0": "Phalehi aṁsehi atīva saṅgatā;",
  "vv64:2.3_0": "Sujātagumbā naravīraniṭṭhitā,",
  "vv64:2.4_0": "Virocatī pannaraseva cando.",
  "vv64:3.1_0": "Suvaṇṇajālāvatato ratho ayaṁ,",
  "vv64:3.2_0": "Bahūhi nānāratanehi cittito;",
  "vv64:3.3_0": "Sunandighoso ca subhassaro ca,",
  "vv64:3.4_0": "Virocatī cāmarahatthabāhubhi.",
  "vv64:4.1_0": "Imā ca nābhyo manasābhinimmitā,",
  "vv64:4.2_0": "Rathassa pādantaramajjhabhūsitā;",
  "vv64:4.3_0": "Imā ca nābhyo satarājicittitā,",
  "vv64:4.4_0": "Sateratā vijjurivappabhāsare.",
  "vv64:5.1_0": "Anekacittāvatato ratho ayaṁ,",
  "vv64:5.2_0": "Puthū ca nemī ca sahassaraṁsiko;",
  "vv64:5.3_0": "Tesaṁ saro suyyati vaggurūpo,",
  "vv64:5.4_0": "Pañcaṅgikaṁ tūriyamivappavāditaṁ.",
  "vv64:6.1_0": "Sirasmiṁ cittaṁ maṇicandakappitaṁ,",
  "vv64:6.2_0": "Sadā visuddhaṁ ruciraṁ pabhassaraṁ;",
  "vv64:6.3_0": "Suvaṇṇarājīhi atīva saṅgataṁ,",
  "vv64:6.4_0": "Veḷuriyarājīva atīva sobhati.",
  "vv64:7.1_0": "Ime ca vāḷī maṇicandakappitā,",
  "vv64:7.2_0": "Ārohakambū sujavā brahūpamā;",
  "vv64:7.3_0": "Brahā mahantā balino mahājavā,",
  "vv64:7.4_0": "Mano tavaññāya tatheva siṁsare.",
  "vv64:8.1_0": "Ime ca sabbe sahitā catukkamā,",
  "vv64:8.2_0": "Mano tavaññāya tatheva siṁsare;",
  "vv64:8.3_0": "Samaṁ vahantī mudukā anuddhatā,",
  "vv64:8.4_0": "Āmodamānā turagānamuttama.",
  "vv64:9.1_0": "Dhunanti vagganti patanti cambare,",
  "vv64:9.2_0": "Abbhuddhunantā sukate piḷandhane;",
  "vv64:9.3_0": "Tesaṁ saro suyyati vaggurūpo,",
  "vv64:9.4_0": "Pañcaṅgikaṁ tūriyamivappavāditaṁ.",
  "vv64:10.1_0": "Rathassa ghoso apiḷandhanāna ca,",
  "vv64:10.2_0": "Khurassa nādo abhihiṁsanāya ca;",
  "vv64:10.3_0": "Ghoso suvaggū samitassa suyyati,",
  "vv64:10.4_0": "Gandhabbatūriyāni vicitrasaṁvane.",
  "vv64:11.1_0": "Rathe ṭhitā tā migamandalocanā,",
  "vv64:11.2_0": "Āḷārapamhā hasitā piyaṁvadā;",
  "vv64:11.3_0": "Veḷuriyajālāvatatā tanucchavā,",
  "vv64:11.4_0": "Sadeva gandhabbasūraggapūjitā.",
  "vv64:12.1_0": "Tā rattarattambarapītavāsasā,",
  "vv64:12.2_0": "Visālanettā abhirattalocanā;",
  "vv64:12.3_0": "Kule sujātā sutanū sucimhitā,",
  "vv64:12.4_0": "Rathe ṭhitā pañjalikā upaṭṭhitā.",
  "vv64:13.1_0": "Tā kambukeyūradharā suvāsasā,",
  "vv64:13.2_0": "Sumajjhimā ūruthanūpapannā;",
  "vv64:13.3_0": "Vaṭṭaṅguliyo sumukhā sudassanā,",
  "vv64:13.4_0": "Rathe ṭhitā pañjalikā upaṭṭhitā.",
  "vv64:14.1_0": "Aññā suveṇī susu missakesiyo,",
  "vv64:14.2_0": "Samaṁ vibhattāhi pabhassarāhi ca;",
  "vv64:14.3_0": "Anubbatā tā tava mānase ratā,",
  "vv64:14.4_0": "Rathe ṭhitā pañjalikā upaṭṭhitā.",
  "vv64:15.1_0": "Āveḷiniyo padumuppalacchadā,",
  "vv64:15.2_0": "Alaṅkatā candanasāravāsitā;",
  "vv64:15.3_0": "Anubbatā tā tava mānase ratā,",
  "vv64:15.4_0": "Rathe ṭhitā pañjalikā upaṭṭhitā.",
  "vv64:16.1_0": "Tā māliniyo padumuppalacchadā,",
  "vv64:16.2_0": "Alaṅkatā candanasāravāsitā;",
  "vv64:16.3_0": "Anubbatā tā tava mānase ratā,",
  "vv64:16.4_0": "Rathe ṭhitā pañjalikā upaṭṭhitā.",
  "vv64:17.1_0": "Kaṇṭhesu te yāni piḷandhanāni,",
  "vv64:17.2_0": "Hatthesu pādesu tatheva sīse;",
  "vv64:17.3_0": "Obhāsayantī dasa sabbaso disā,",
  "vv64:17.4_0": "Abbhuddayaṁ sāradikova bhāṇumā.",
  "vv64:18.1_0": "Vātassa vegena ca sampakampitā,",
  "vv64:18.2_0": "Bhujesu mālā apiḷandhanāni ca;",
  "vv64:18.3_0": "Muñcanti ghosaṁ ruciraṁ suciṁ subhaṁ,",
  "vv64:18.4_0": "Sabbehi viññūhi sutabbarūpaṁ.",
  "vv64:19.1_0": "Uyyānabhūmyā ca duvaddhato ṭhitā,",
  "vv64:19.2_0": "Rathā ca nāgā tūriyāni ca saro;",
  "vv64:19.3_0": "Tameva devinda pamodayanti,",
  "vv64:19.4_0": "Vīṇā yathā pokkharapattabāhubhi.",
  "vv64:20.1_0": "Imāsu vīṇāsu bahūsu vaggūsu,",
  "vv64:20.2_0": "Manuññarūpāsu hadayeritaṁ pītiṁ;",
  "vv64:20.3_0": "Pavajjamānāsu atīva accharā,",
  "vv64:20.4_0": "Bhamanti kaññā padumesu sikkhitā.",
  "vv64:21.1_0": "Yadā ca gītāni ca vāditāni ca,",
  "vv64:21.2_0": "Naccāni cimāni samenti ekato;",
  "vv64:21.3_0": "Athettha naccanti athettha accharā,",
  "vv64:21.4_0": "Obhāsayantī ubhato varitthiyo.",
  "vv64:22.1_0": "So modasi tūriyagaṇappabodhano,",
  "vv64:22.2_0": "Mahīyamāno vajirāvudhoriva;",
  "vv64:22.3_0": "Imāsu vīṇāsu bahūsu vaggūsu,",
  "vv64:22.4_0": "Manuññarūpāsu hadayeritaṁ pītiṁ.",
  "vv64:23.1_0": "Kiṁ tvaṁ pure kammamakāsi attanā,",
  "vv64:23.2_0": "Manussabhūto purimāya jātiyā;",
  "vv64:23.3_0": "Uposathaṁ kaṁ vā tuvaṁ upāvasi,",
  "vv64:23.4_0": "Kaṁ dhammacariyaṁ vatamābhirocayi.",
  "vv64:24.1_0": "Nayīdamappassa katassa kammuno,",
  "vv64:24.2_0": "Pubbe suciṇṇassa uposathassa vā;",
  "vv64:24.3_0": "Iddhānubhāvo vipulo ayaṁ tava,",
  "vv64:24.4_0": "Yaṁ devasaṅghaṁ abhirocase bhusaṁ.",
  "vv64:25.1_0": "Dānassa te idaṁ phalaṁ,",
  "vv64:25.2_0": "atho sīlassa vā pana;",
  "vv64:25.3_0": "Atho añjalikammassa,",
  "vv64:25.4_0": "taṁ me akkhāhi pucchito”ti.",
  "vv64:26.1_0": "So devaputto attamano,",
  "vv64:26.2_0": "moggallānena pucchito;",
  "vv64:26.3_0": "Pañhaṁ puṭṭho viyākāsi,",
  "vv64:26.4_0": "yassa kammassidaṁ phalanti.",
  "vv64:27.1_0": "“Jitindriyaṁ buddhamanomanikkamaṁ,",
  "vv64:27.2_0": "Naruttamaṁ kassapamaggapuggalaṁ;",
  "vv64:27.3_0": "Avāpurantaṁ amatassa dvāraṁ,",
  "vv64:27.4_0": "Devātidevaṁ satapuññalakkhaṇaṁ.",
  "vv64:28.1_0": "Tamaddasaṁ kuñjaramoghatiṇṇaṁ,",
  "vv64:28.2_0": "Suvaṇṇasiṅgīnadabimbasādisaṁ;",
  "vv64:28.3_0": "Disvāna taṁ khippamahuṁ sucīmano,",
  "vv64:28.4_0": "Tameva disvāna subhāsitaddhajaṁ.",
  "vv64:29.1_0": "Tamannapānaṁ athavāpi cīvaraṁ,",
  "vv64:29.2_0": "Suciṁ paṇītaṁ rasasā upetaṁ;",
  "vv64:29.3_0": "Pupphābhikkiṇamhi sake nivesane,",
  "vv64:29.4_0": "Patiṭṭhapesiṁ sa asaṅgamānaso.",
  "vv64:30.1_0": "Tamannapānena ca cīvarena ca,",
  "vv64:30.2_0": "Khajjena bhojjena ca sāyanena ca;",
  "vv64:30.3_0": "Santappayitvā dvipadānamuttamaṁ,",
  "vv64:30.4_0": "So saggaso devapure ramāmahaṁ.",
  "vv64:31.1_0": "Etenupāyena imaṁ niraggaḷaṁ,",
  "vv64:31.2_0": "Yaññaṁ yajitvā tividhaṁ visuddhaṁ;",
  "vv64:31.3_0": "Pahāyahaṁ mānusakaṁ samussayaṁ,",
  "vv64:31.4_0": "Indūpamo devapure ramāmahaṁ.",
  "vv64:32.1_0": "Āyuñca vaṇṇañca sukhaṁ balañca,",
  "vv64:32.2_0": "Paṇītarūpaṁ abhikaṅkhatā muni;",
  "vv64:32.3_0": "Annañca pānañca bahuṁ susaṅkhataṁ,",
  "vv64:32.4_0": "Patiṭṭhapetabbamasaṅgamānase.",
  "vv64:33.1_0": "Nayimasmiṁ loke parasmiṁ vā pana,",
  "vv64:33.2_0": "Buddhena seṭṭho va samo va vijjati;",
  "vv64:33.3_0": "Āhuneyyānaṁ paramāhutiṁ gato,",
  "vv64:33.4_0": "Puññatthikānaṁ vipulapphalesinan”ti.",
  "vv64:34.1_0": "Mahārathavimānaṁ cuddasamaṁ.",
  "vv64:34.2_0": "Mahārathavaggo pañcamo.",
  "vv64:35.1_0": "Tassuddānaṁ",
  "vv64:36.1_0": "Maṇḍūko revatī chatto,",
  "vv64:36.2_0": "Kakkaṭo dvārapālako;",
  "vv64:36.3_0": "Dve karaṇīyā dve sūci,",
  "vv64:36.4_0": "Tayo nāgā ca dve rathā;",
  "vv64:36.5_0": "Purisānaṁ paṭhamo vaggo pavuccatīti.",
  "vv64:37.1_0": "Bhāṇavāraṁ tatiyaṁ."
}