{
  "thi-ap35:0.1_0": "Therīapadāna",
  "thi-ap35:0.2_0": "Khattiyāvagga",
  "thi-ap35:0.3_0": "5. Sukkātherīapadāna",
  "thi-ap35:1.1_0": "“Ekanavutito kappe,",
  "thi-ap35:1.2_0": "vipassī nāma nāyako;",
  "thi-ap35:1.3_0": "Uppajji cārudassano,",
  "thi-ap35:1.4_0": "sabbadhammavipassako.",
  "thi-ap35:2.1_0": "Tadāhaṁ bandhumatiyaṁ,",
  "thi-ap35:2.2_0": "jātā aññatare kule;",
  "thi-ap35:2.3_0": "Dhammaṁ sutvāna munino,",
  "thi-ap35:2.4_0": "pabbajiṁ anagāriyaṁ.",
  "thi-ap35:3.1_0": "Bahussutā dhammadharā,",
  "thi-ap35:3.2_0": "paṭibhānavatī tathā;",
  "thi-ap35:3.3_0": "Vicittakathikā cāpi,",
  "thi-ap35:3.4_0": "jinasāsanakārikā.",
  "thi-ap35:4.1_0": "Tadā dhammakathaṁ katvā,",
  "thi-ap35:4.2_0": "hitāya janataṁ bahuṁ;",
  "thi-ap35:4.3_0": "Tato cutāhaṁ tusitaṁ,",
  "thi-ap35:4.4_0": "upapannā yasassinī.",
  "thi-ap35:5.1_0": "Ekattiṁse ito kappe,",
  "thi-ap35:5.2_0": "sikhī viya sikhī jino;",
  "thi-ap35:5.3_0": "Tapanto yasasā loke,",
  "thi-ap35:5.4_0": "uppajji vadataṁ varo.",
  "thi-ap35:6.1_0": "Tadāpi pabbajitvāna,",
  "thi-ap35:6.2_0": "buddhasāsanakovidā;",
  "thi-ap35:6.3_0": "Jotetvā jinavākyāni,",
  "thi-ap35:6.4_0": "tatopi tidivaṁ gatā.",
  "thi-ap35:7.1_0": "Ekattiṁseva kappamhi,",
  "thi-ap35:7.2_0": "vessabhū nāma nāyako;",
  "thi-ap35:7.3_0": "Uppajjittha mahāñāṇī,",
  "thi-ap35:7.4_0": "tadāpi ca tathevahaṁ.",
  "thi-ap35:8.1_0": "Pabbajitvā dhammadharā,",
  "thi-ap35:8.2_0": "jotayiṁ jinasāsanaṁ;",
  "thi-ap35:8.3_0": "Gantvā marupuraṁ rammaṁ,",
  "thi-ap35:8.4_0": "anubhosiṁ mahāsukhaṁ.",
  "thi-ap35:9.1_0": "Imamhi bhaddake kappe,",
  "thi-ap35:9.2_0": "kakusandho jinuttamo;",
  "thi-ap35:9.3_0": "Uppajji narasaraṇo,",
  "thi-ap35:9.4_0": "tadāpi ca tathevahaṁ.",
  "thi-ap35:10.1_0": "Pabbajitvā munimataṁ,",
  "thi-ap35:10.2_0": "jotayitvā yathāyukaṁ;",
  "thi-ap35:10.3_0": "Tato cutāhaṁ tidivaṁ,",
  "thi-ap35:10.4_0": "agaṁ sabhavanaṁ yathā.",
  "thi-ap35:11.1_0": "Imasmiṁyeva kappamhi,",
  "thi-ap35:11.2_0": "koṇāgamananāyako;",
  "thi-ap35:11.3_0": "Uppajji lokasaraṇo,",
  "thi-ap35:11.4_0": "araṇo amataṅgato.",
  "thi-ap35:12.1_0": "Tadāpi pabbajitvāna,",
  "thi-ap35:12.2_0": "sāsane tassa tādino;",
  "thi-ap35:12.3_0": "Bahussutā dhammadharā,",
  "thi-ap35:12.4_0": "jotayiṁ jinasāsanaṁ.",
  "thi-ap35:13.1_0": "Imasmiṁyeva kappamhi,",
  "thi-ap35:13.2_0": "kassapo munimuttamo;",
  "thi-ap35:13.3_0": "Uppajji lokasaraṇo,",
  "thi-ap35:13.4_0": "araṇo maraṇantagū.",
  "thi-ap35:14.1_0": "Tassāpi naravīrassa,",
  "thi-ap35:14.2_0": "pabbajitvāna sāsane;",
  "thi-ap35:14.3_0": "Pariyāpuṭasaddhammā,",
  "thi-ap35:14.4_0": "paripucchā visāradā.",
  "thi-ap35:15.1_0": "Susīlā lajjinī ceva,",
  "thi-ap35:15.2_0": "tīsu sikkhāsu kovidā;",
  "thi-ap35:15.3_0": "Bahuṁ dhammakathaṁ katvā,",
  "thi-ap35:15.4_0": "yāvajīvaṁ mahāmune.",
  "thi-ap35:16.1_0": "Tena kammavipākena,",
  "thi-ap35:16.2_0": "cetanāpaṇidhīhi ca;",
  "thi-ap35:16.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "thi-ap35:16.4_0": "tāvatiṁsamagacchahaṁ.",
  "thi-ap35:17.1_0": "Pacchime ca bhave dāni,",
  "thi-ap35:17.2_0": "giribbajapuruttame;",
  "thi-ap35:17.3_0": "Jātā seṭṭhikule phīte,",
  "thi-ap35:17.4_0": "mahāratanasañcaye.",
  "thi-ap35:18.1_0": "Yadā bhikkhusahassena,",
  "thi-ap35:18.2_0": "parivuto lokanāyako;",
  "thi-ap35:18.3_0": "Upāgami rājagahaṁ,",
  "thi-ap35:18.4_0": "sahassakkhena vaṇṇito.",
  "thi-ap35:19.1_0": "Danto dantehi saha purāṇajaṭilehi,",
  "thi-ap35:19.2_0": "Vippamutto vippamuttehi;",
  "thi-ap35:19.3_0": "Siṅgīnikkhasavaṇṇo,",
  "thi-ap35:19.4_0": "Rājagahaṁ pāvisi bhagavā.",
  "thi-ap35:20.1_0": "Disvā buddhānubhāvaṁ taṁ,",
  "thi-ap35:20.2_0": "sutvāva guṇasañcayaṁ;",
  "thi-ap35:20.3_0": "Buddhe cittaṁ pasādetvā,",
  "thi-ap35:20.4_0": "pūjayiṁ taṁ yathābalaṁ.",
  "thi-ap35:21.1_0": "Aparena ca kālena,",
  "thi-ap35:21.2_0": "dhammadinnāya santike;",
  "thi-ap35:21.3_0": "Agārā nikkhamitvāna,",
  "thi-ap35:21.4_0": "pabbajiṁ anagāriyaṁ.",
  "thi-ap35:22.1_0": "Kesesu chijjamānesu,",
  "thi-ap35:22.2_0": "kilese jhāpayiṁ ahaṁ;",
  "thi-ap35:22.3_0": "Uggahiṁ sāsanaṁ sabbaṁ,",
  "thi-ap35:22.4_0": "pabbajitvā cirenahaṁ.",
  "thi-ap35:23.1_0": "Tato dhammamadesesiṁ,",
  "thi-ap35:23.2_0": "mahājanasamāgame;",
  "thi-ap35:23.3_0": "Dhamme desiyamānamhi,",
  "thi-ap35:23.4_0": "dhammābhisamayo ahu.",
  "thi-ap35:24.1_0": "Nekapāṇasahassānaṁ,",
  "thi-ap35:24.2_0": "taṁ viditvātivimhito;",
  "thi-ap35:24.3_0": "Abhippasanno me yakkho,",
  "thi-ap35:24.4_0": "bhamitvāna giribbajaṁ.",
  "thi-ap35:25.1_0": "Kiṁ me katā rājagahe manussā,",
  "thi-ap35:25.2_0": "Madhuṁ pītāva acchare;",
  "thi-ap35:25.3_0": "Ye sukkaṁ na upāsanti,",
  "thi-ap35:25.4_0": "Desentiṁ amataṁ padaṁ.",
  "thi-ap35:26.1_0": "Tañca appaṭivānīyaṁ,",
  "thi-ap35:26.2_0": "asecanakamojavaṁ;",
  "thi-ap35:26.3_0": "Pivanti maññe sappaññā,",
  "thi-ap35:26.4_0": "valāhakamivaddhagū.",
  "thi-ap35:27.1_0": "Iddhīsu ca vasī homi,",
  "thi-ap35:27.2_0": "dibbāya sotadhātuyā;",
  "thi-ap35:27.3_0": "Cetopariyañāṇassa,",
  "thi-ap35:27.4_0": "vasī homi mahāmune.",
  "thi-ap35:28.1_0": "Pubbenivāsaṁ jānāmi,",
  "thi-ap35:28.2_0": "dibbacakkhu visodhitaṁ;",
  "thi-ap35:28.3_0": "Sabbāsavā parikkhīṇā,",
  "thi-ap35:28.4_0": "natthi dāni punabbhavo.",
  "thi-ap35:29.1_0": "Atthadhammaniruttīsu,",
  "thi-ap35:29.2_0": "paṭibhāne tatheva ca;",
  "thi-ap35:29.3_0": "Ñāṇaṁ mama mahāvīra,",
  "thi-ap35:29.4_0": "uppannaṁ tava santike.",
  "thi-ap35:30.1_0": "Kilesā jhāpitā mayhaṁ,",
  "thi-ap35:30.2_0": "…pe…",
  "thi-ap35:30.3_0": "viharāmi anāsavā.",
  "thi-ap35:31.1_0": "Svāgataṁ vata me āsi,",
  "thi-ap35:31.2_0": "…pe…",
  "thi-ap35:31.3_0": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap35:32.1_0": "Paṭisambhidā catasso,",
  "thi-ap35:32.2_0": "…pe…",
  "thi-ap35:32.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "thi-ap35:33.1_0": "Itthaṁ sudaṁ sukkā bhikkhunī imā gāthāyo abhāsitthāti.",
  "thi-ap35:34.1_0": "Sukkātheriyāpadānaṁ pañcamaṁ.",
  "thi-ap35:34.2_0": "Pañcamaṁ bhāṇavāraṁ."
}