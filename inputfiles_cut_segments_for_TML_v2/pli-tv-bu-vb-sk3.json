{
  "pli-tv-bu-vb-sk3:0.1_0": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk3:0.2_0": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk3:0.3_0": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk3:0.4_0": "Parimaṇḍalavagga",
  "pli-tv-bu-vb-sk3:0.5_0": "3. Suppaṭicchannasikkhāpada",
  "pli-tv-bu-vb-sk3:1.1_0": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk3:1.2_0": "Tena kho pana samayena chabbaggiyā bhikkhū kāyaṁ vivaritvā antaraghare gacchanti …pe….",
  "pli-tv-bu-vb-sk3:1.3.1_0": "“Suppaṭicchanno antaraghare gamissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk3:1.4_0": "Suppaṭicchannena antaraghare gantabbaṁ.",
  "pli-tv-bu-vb-sk3:1.5_0": "Yo anādariyaṁ paṭicca kāyaṁ vivaritvā antaraghare gacchati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk3:1.6.1_0": "Anāpatti—",
  "pli-tv-bu-vb-sk3:1.7_0": "asañcicca,",
  "pli-tv-bu-vb-sk3:1.8_0": "assatiyā,",
  "pli-tv-bu-vb-sk3:1.9_0": "ajānantassa,",
  "pli-tv-bu-vb-sk3:1.10_0": "gilānassa,",
  "pli-tv-bu-vb-sk3:1.11_0": "āpadāsu,",
  "pli-tv-bu-vb-sk3:1.12_0": "ummattakassa,",
  "pli-tv-bu-vb-sk3:1.13_0": "ādikammikassāti.",
  "pli-tv-bu-vb-sk3:1.14_0": "Tatiyasikkhāpadaṁ niṭṭhitaṁ."
}