{
  "iti112:0.1_0": "Itivuttaka 112",
  "iti112:0.2_0": "Catukkanipāta",
  "iti112:0.3_0": "Brāhmaṇadhammayāgavagga",
  "iti112:0.4_0": "Lokasutta",
  "iti112:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti112:2.1_0": "“Loko, bhikkhave, tathāgatena abhisambuddho,",
  "iti112:2.2_0": "lokasmā tathāgato visaṁyutto.",
  "iti112:2.3_0": "Lokasamudayo, bhikkhave, tathāgatena abhisambuddho,",
  "iti112:2.4_0": "lokasamudayo tathāgatassa pahīno.",
  "iti112:2.5_0": "Lokanirodho, bhikkhave, tathāgatena abhisambuddho,",
  "iti112:2.6_0": "lokanirodho tathāgatassa sacchikato.",
  "iti112:2.7_0": "Lokanirodhagāminī paṭipadā, bhikkhave, tathāgatena abhisambuddhā,",
  "iti112:2.8_0": "lokanirodhagāminī paṭipadā tathāgatassa bhāvitā.",
  "iti112:3.1_0": "Yaṁ, bhikkhave, sadevakassa lokassa samārakassa sabrahmakassa sassamaṇabrāhmaṇiyā pajāya sadevamanussāya diṭṭhaṁ sutaṁ mutaṁ viññātaṁ pattaṁ pariyesitaṁ anuvicaritaṁ manasā yasmā taṁ tathāgatena abhisambuddhaṁ,",
  "iti112:3.2_0": "tasmā ‘tathāgato’ti vuccati.",
  "iti112:4.1_0": "Yañca, bhikkhave, rattiṁ tathāgato anuttaraṁ sammāsambodhiṁ abhisambujjhati, yañca rattiṁ anupādisesāya nibbānadhātuyā parinibbāyati, yaṁ etasmiṁ antare bhāsati lapati niddisati, sabbaṁ taṁ tatheva hoti no aññathā,",
  "iti112:4.2_0": "tasmā ‘tathāgato’ti vuccati.",
  "iti112:5.1_0": "Yathāvādī, bhikkhave, tathāgato tathākārī, yathākārī tathāvādī,",
  "iti112:5.2_0": "iti yathāvādī tathākārī yathākārī tathāvādī,",
  "iti112:5.3_0": "tasmā ‘tathāgato’ti vuccati.",
  "iti112:6.1_0": "Sadevake, bhikkhave, loke samārake sabrahmake sassamaṇabrāhmaṇiyā pajāya sadevamanussāya tathāgato abhibhū anabhibhūto aññadatthudaso vasavattī,",
  "iti112:6.2_0": "tasmā tathāgatoti vuccatī”ti.",
  "iti112:6.3_0": "Etamatthaṁ bhagavā avoca.",
  "iti112:6.4_0": "Tatthetaṁ iti vuccati:",
  "iti112:7.1_0": "“Sabbalokaṁ abhiññāya,",
  "iti112:7.2_0": "sabbaloke yathātathaṁ;",
  "iti112:7.3_0": "Sabbalokavisaṁyutto,",
  "iti112:7.4_0": "sabbaloke anūpayo.",
  "iti112:8.1_0": "Sa ve sabbābhibhū dhīro,",
  "iti112:8.2_0": "sabbaganthappamocano;",
  "iti112:8.3_0": "Phuṭṭhāssa paramā santi,",
  "iti112:8.4_0": "nibbānaṁ akutobhayaṁ.",
  "iti112:9.1_0": "Esa khīṇāsavo buddho,",
  "iti112:9.2_0": "anīgho chinnasaṁsayo;",
  "iti112:9.3_0": "Sabbakammakkhayaṁ patto,",
  "iti112:9.4_0": "vimutto upadhisaṅkhaye.",
  "iti112:10.1_0": "Esa so bhagavā buddho,",
  "iti112:10.2_0": "esa sīho anuttaro;",
  "iti112:10.3_0": "Sadevakassa lokassa,",
  "iti112:10.4_0": "brahmacakkaṁ pavattayi.",
  "iti112:11.1_0": "Iti devā manussā ca,",
  "iti112:11.2_0": "ye buddhaṁ saraṇaṁ gatā;",
  "iti112:11.3_0": "Saṅgamma taṁ namassanti,",
  "iti112:11.4_0": "mahantaṁ vītasāradaṁ.",
  "iti112:12.1_0": "Danto damayataṁ seṭṭho,",
  "iti112:12.2_0": "santo samayataṁ isi;",
  "iti112:12.3_0": "Mutto mocayataṁ aggo,",
  "iti112:12.4_0": "tiṇṇo tārayataṁ varo.",
  "iti112:13.1_0": "Iti hetaṁ namassanti,",
  "iti112:13.2_0": "mahantaṁ vītasāradaṁ;",
  "iti112:13.3_0": "Sadevakasmiṁ lokasmiṁ,",
  "iti112:13.4_0": "natthi te paṭipuggalo”ti.",
  "iti112:14.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti112:14.2_0": "Terasamaṁ.",
  "iti112:14.3_0": "Catukkanipāto niṭṭhito.",
  "iti112:15.0_0": "Tassuddānaṁ",
  "iti112:15.1_0": "Brāhmaṇasulabhā jānaṁ,",
  "iti112:15.2_0": "Samaṇasīlā taṇhā brahmā;",
  "iti112:15.3_0": "Bahukārā kuhapurisā,",
  "iti112:15.4_0": "Cara sampannalokena terasāti.",
  "iti112:16.0_0": "Suttasaṅgaho",
  "iti112:16.1_0": "Sattavisekanipātaṁ,",
  "iti112:16.2_0": "Dukkaṁ bāvīsasuttasaṅgahitaṁ;",
  "iti112:16.3_0": "Samapaññāsamathatikaṁ,",
  "iti112:16.4_0": "Terasa catukkañca iti yamidaṁ.",
  "iti112:17.1_0": "Dvidasuttarasuttasate,",
  "iti112:17.2_0": "Saṅgāyitvā samādahiṁsu purā;",
  "iti112:17.3_0": "Arahanto ciraṭṭhitiyā,",
  "iti112:17.4_0": "Tamāhu nāmena itivuttanti.",
  "iti112:17.5_0": "Itivuttakapāḷi niṭṭhitā."
}