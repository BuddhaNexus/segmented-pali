{
  "ja463:0.1_0": "Jātaka",
  "ja463:0.2_0": "Ekādasakanipāta",
  "ja463:0.3_0": "Mātuposakavagga",
  "ja463:0.4_0": "9. Suppārakajātaka",
  "ja463:1.1_0": "“Ummujjanti nimujjanti,",
  "ja463:1.2_0": "manussā khuranāsikā;",
  "ja463:1.3_0": "Suppārakaṁ taṁ pucchāma,",
  "ja463:1.4_0": "samuddo katamo ayaṁ”.",
  "ja463:2.1_0": "“Kurukacchā payātānaṁ,",
  "ja463:2.2_0": "vāṇijānaṁ dhanesinaṁ;",
  "ja463:2.3_0": "Nāvāya vippanaṭṭhāya,",
  "ja463:2.4_0": "khuramālīti vuccati”.",
  "ja463:3.1_0": "“Yathā aggīva suriyova,",
  "ja463:3.2_0": "samuddo paṭidissati;",
  "ja463:3.3_0": "Suppārakaṁ taṁ pucchāma,",
  "ja463:3.4_0": "samuddo katamo ayaṁ”.",
  "ja463:4.1_0": "“Kurukacchā payātānaṁ,",
  "ja463:4.2_0": "vāṇijānaṁ dhanesinaṁ;",
  "ja463:4.3_0": "Nāvāya vippanaṭṭhāya,",
  "ja463:4.4_0": "aggimālīti vuccati”.",
  "ja463:5.1_0": "“Yathā dadhīva khīraṁva,",
  "ja463:5.2_0": "samuddo paṭidissati;",
  "ja463:5.3_0": "Suppārakaṁ taṁ pucchāma,",
  "ja463:5.4_0": "samuddo katamo ayaṁ”.",
  "ja463:6.1_0": "“Kurukacchā payātānaṁ,",
  "ja463:6.2_0": "vāṇijānaṁ dhanesinaṁ;",
  "ja463:6.3_0": "Nāvāya vippanaṭṭhāya,",
  "ja463:6.4_0": "dadhimālīti vuccati”.",
  "ja463:7.1_0": "“Yathā kusova sassova,",
  "ja463:7.2_0": "samuddo paṭidissati;",
  "ja463:7.3_0": "Suppārakaṁ taṁ pucchāma,",
  "ja463:7.4_0": "samuddo katamo ayaṁ”.",
  "ja463:8.1_0": "“Kurukacchā payātānaṁ,",
  "ja463:8.2_0": "vāṇijānaṁ dhanesinaṁ;",
  "ja463:8.3_0": "Nāvāya vippanaṭṭhāya,",
  "ja463:8.4_0": "kusamālīti vuccati”.",
  "ja463:9.1_0": "“Yathā naḷova veḷūva,",
  "ja463:9.2_0": "samuddo paṭidissati;",
  "ja463:9.3_0": "Suppārakaṁ taṁ pucchāma,",
  "ja463:9.4_0": "samuddo katamo ayaṁ”.",
  "ja463:10.1_0": "“Kurukacchā payātānaṁ,",
  "ja463:10.2_0": "vāṇijānaṁ dhanesinaṁ;",
  "ja463:10.3_0": "Nāvāya vippanaṭṭhāya,",
  "ja463:10.4_0": "naḷamālīti vuccati”.",
  "ja463:11.1_0": "“Mahabbhayo bhiṁsanako,",
  "ja463:11.2_0": "saddo suyyatimānuso;",
  "ja463:11.3_0": "Yathā sobbho papātova,",
  "ja463:11.4_0": "samuddo paṭidissati;",
  "ja463:11.5_0": "Suppārakaṁ taṁ pucchāma,",
  "ja463:11.6_0": "samuddo katamo ayaṁ”.",
  "ja463:12.1_0": "“Kurukacchā payātānaṁ,",
  "ja463:12.2_0": "vāṇijānaṁ dhanesinaṁ;",
  "ja463:12.3_0": "Nāvāya vippanaṭṭhāya,",
  "ja463:12.4_0": "baḷavāmukhīti vuccati.",
  "ja463:13.1_0": "Yato sarāmi attānaṁ,",
  "ja463:13.2_0": "Yato pattosmi viññutaṁ;",
  "ja463:13.3_0": "Nābhijānāmi sañcicca,",
  "ja463:13.4_0": "Ekapāṇampi hiṁsitaṁ;",
  "ja463:13.5_0": "Etena saccavajjena,",
  "ja463:13.6_0": "Sotthiṁ nāvā nivattatū”ti.",
  "ja463:14.1_0": "Suppārakajātakaṁ navamaṁ.",
  "ja463:15.1_0": "Ekādasakanipātaṁ niṭṭhitaṁ.",
  "ja463:16.1_0": "Tassuddānaṁ",
  "ja463:17.1_0": "Sirimātusuposakanāgavaro,",
  "ja463:17.2_0": "Puna juṇhaka dhammamudayavaro;",
  "ja463:17.3_0": "Atha pāni yudhañcayako ca dasa-",
  "ja463:17.4_0": "Ratha saṁvara pāragatena navāti."
}