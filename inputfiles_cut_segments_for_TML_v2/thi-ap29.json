{
  "thi-ap29:0.1_0": "Therīapadāna",
  "thi-ap29:0.2_0": "Kuṇḍalakesīvagga",
  "thi-ap29:0.3_0": "9 Yasodharāpamukhadasabhikkhunīsahassaapadāna",
  "thi-ap29:1.1_0": "“Kappe ca satasahasse,",
  "thi-ap29:1.2_0": "caturo ca asaṅkhiye;",
  "thi-ap29:1.3_0": "Dīpaṅkaro nāma jino,",
  "thi-ap29:1.4_0": "uppajji lokanāyako.",
  "thi-ap29:2.1_0": "Dīpaṅkaro mahāvīro,",
  "thi-ap29:2.2_0": "viyākāsi vināyako;",
  "thi-ap29:2.3_0": "Sumedhañca sumittañca,",
  "thi-ap29:2.4_0": "samānasukhadukkhataṁ.",
  "thi-ap29:3.1_0": "Sadevakañca passanto,",
  "thi-ap29:3.2_0": "vicaranto sadevakaṁ;",
  "thi-ap29:3.3_0": "Tesaṁ pakittane amhe,",
  "thi-ap29:3.4_0": "upagamma samāgamaṁ.",
  "thi-ap29:4.1_0": "Amhaṁ sabbapati hohi,",
  "thi-ap29:4.2_0": "anāgatasamāgame;",
  "thi-ap29:4.3_0": "Sabbāva tuyhaṁ bhariyā,",
  "thi-ap29:4.4_0": "manāpā piyavādikā.",
  "thi-ap29:5.1_0": "Dānaṁ sīlamayaṁ sabbaṁ,",
  "thi-ap29:5.2_0": "bhāvanā ca subhāvitā;",
  "thi-ap29:5.3_0": "Dīgharattañca no sabbaṁ,",
  "thi-ap29:5.4_0": "pariccattaṁ mahāmune.",
  "thi-ap29:6.1_0": "Gandhaṁ vilepanaṁ mālaṁ,",
  "thi-ap29:6.2_0": "dīpañca ratanāmayaṁ;",
  "thi-ap29:6.3_0": "Yaṁ kiñci patthitaṁ sabbaṁ,",
  "thi-ap29:6.4_0": "pariccattaṁ mahāmuni.",
  "thi-ap29:7.1_0": "Aññaṁ vāpi kataṁ kammaṁ,",
  "thi-ap29:7.2_0": "paribhogañca mānusaṁ;",
  "thi-ap29:7.3_0": "Dīgharattañhi no sabbaṁ,",
  "thi-ap29:7.4_0": "pariccattaṁ mahāmuni.",
  "thi-ap29:8.1_0": "Anekajātisaṁsāraṁ,",
  "thi-ap29:8.2_0": "bahuṁ puññampi no kataṁ;",
  "thi-ap29:8.3_0": "Issaramanubhotvāna,",
  "thi-ap29:8.4_0": "saṁsaritvā bhavābhave.",
  "thi-ap29:9.1_0": "Pacchime bhave sampatte,",
  "thi-ap29:9.2_0": "sakyaputtanivesane;",
  "thi-ap29:9.3_0": "Nānākulūpapannāyo,",
  "thi-ap29:9.4_0": "accharā kāmavaṇṇinī.",
  "thi-ap29:10.1_0": "Lābhaggena yasaṁ pattā,",
  "thi-ap29:10.2_0": "pūjitā sabbasakkatā;",
  "thi-ap29:10.3_0": "Lābhiyo annapānānaṁ,",
  "thi-ap29:10.4_0": "sadā sammānitā mayaṁ.",
  "thi-ap29:11.1_0": "Agāraṁ pajahitvāna,",
  "thi-ap29:11.2_0": "pabbajimhanagāriyaṁ;",
  "thi-ap29:11.3_0": "Aḍḍhamāse asampatte,",
  "thi-ap29:11.4_0": "sabbā pattāmha nibbutiṁ.",
  "thi-ap29:12.1_0": "Lābhiyo annapānānaṁ,",
  "thi-ap29:12.2_0": "vatthasenāsanāni ca;",
  "thi-ap29:12.3_0": "Upenti paccayā sabbe,",
  "thi-ap29:12.4_0": "sadā sakkatapūjitā.",
  "thi-ap29:13.1_0": "Kilesā jhāpitā amhaṁ,",
  "thi-ap29:13.2_0": "…pe…",
  "thi-ap29:13.3_0": "viharāma anāsavā.",
  "thi-ap29:14.1_0": "Svāgataṁ vata no āsi,",
  "thi-ap29:14.2_0": "…pe…",
  "thi-ap29:14.3_0": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap29:15.1_0": "Paṭisambhidā catasso,",
  "thi-ap29:15.2_0": "…pe…",
  "thi-ap29:15.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "thi-ap29:16.1_0": "Itthaṁ sudaṁ yasodharāpamukhāni dasabhikkhunīsahassāni bhagavato sammukhā imā gāthāyo abhāsitthāti.",
  "thi-ap29:17.1_0": "Yasodharāpamukhadasabhikkhunīsahassāpadānaṁ navamaṁ."
}