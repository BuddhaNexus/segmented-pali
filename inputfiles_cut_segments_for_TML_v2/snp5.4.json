{
  "snp5.4:0.1_0": "Sutta Nipāta 5.4",
  "snp5.4:0.2_0": "Puṇṇakamāṇavapucchā",
  "snp5.4:1.1_0": "“Anejaṁ mūladassāviṁ,",
  "snp5.4:1.2_0": "(iccāyasmā puṇṇako)",
  "snp5.4:1.3_0": "Atthi pañhena āgamaṁ;",
  "snp5.4:1.4_0": "Kiṁ nissitā isayo manujā,",
  "snp5.4:1.5_0": "Khattiyā brāhmaṇā devatānaṁ;",
  "snp5.4:1.6_0": "Yaññamakappayiṁsu puthūdha loke,",
  "snp5.4:1.7_0": "Pucchāmi taṁ bhagavā brūhi me taṁ”.",
  "snp5.4:2.1_0": "“Ye kecime isayo manujā,",
  "snp5.4:2.2_0": "(puṇṇakāti bhagavā)",
  "snp5.4:2.3_0": "Khattiyā brāhmaṇā devatānaṁ;",
  "snp5.4:2.4_0": "Yaññamakappayiṁsu puthūdha loke,",
  "snp5.4:2.5_0": "Āsīsamānā puṇṇaka itthattaṁ;",
  "snp5.4:2.6_0": "Jaraṁ sitā yaññamakappayiṁsu”.",
  "snp5.4:3.1_0": "“Ye kecime isayo manujā,",
  "snp5.4:3.2_0": "(iccāyasmā puṇṇako)",
  "snp5.4:3.3_0": "Khattiyā brāhmaṇā devatānaṁ;",
  "snp5.4:3.4_0": "Yaññamakappayiṁsu puthūdha loke,",
  "snp5.4:3.5_0": "Kaccissu te bhagavā yaññapathe appamattā;",
  "snp5.4:3.6_0": "Atāruṁ jātiñca jarañca mārisa,",
  "snp5.4:3.7_0": "Pucchāmi taṁ bhagavā brūhi me taṁ”.",
  "snp5.4:4.1_0": "“Āsīsanti thomayanti abhijappanti juhanti,",
  "snp5.4:4.2_0": "(puṇṇakāti bhagavā)",
  "snp5.4:4.3_0": "Kāmābhijappanti paṭicca lābhaṁ;",
  "snp5.4:4.4_0": "Te yājayogā bhavarāgarattā,",
  "snp5.4:4.5_0": "Nātariṁsu jātijaranti brūmi”.",
  "snp5.4:5.1_0": "“Te ce nātariṁsu yājayogā,",
  "snp5.4:5.2_0": "(iccāyasmā puṇṇako)",
  "snp5.4:5.3_0": "Yaññehi jātiñca jarañca mārisa;",
  "snp5.4:5.4_0": "Atha ko carahi devamanussaloke,",
  "snp5.4:5.5_0": "Atāri jātiñca jarañca mārisa;",
  "snp5.4:5.6_0": "Pucchāmi taṁ bhagavā brūhi me taṁ”.",
  "snp5.4:6.1_0": "“Saṅkhāya lokasmi paroparāni,",
  "snp5.4:6.2_0": "(puṇṇakāti bhagavā)",
  "snp5.4:6.3_0": "Yassiñjitaṁ natthi kuhiñci loke;",
  "snp5.4:6.4_0": "Santo vidhūmo anīgho nirāso,",
  "snp5.4:6.5_0": "Atāri so jātijaranti brūmī”ti.",
  "snp5.4:6.6_0": "Puṇṇakamāṇavapucchā tatiyā."
}