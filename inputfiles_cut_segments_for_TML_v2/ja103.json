{
  "ja103:0.1_0": "Jātaka",
  "ja103:0.2_0": "Ekakanipāta",
  "ja103:0.3_0": "Parosatavagga",
  "ja103:0.4_0": "3. Verijātaka",
  "ja103:1.1_0": "“Yattha verī nivisati,",
  "ja103:1.2_0": "na vase tattha paṇḍito;",
  "ja103:1.3_0": "Ekarattaṁ dirattaṁ vā,",
  "ja103:1.4_0": "dukkhaṁ vasati verisū”ti.",
  "ja103:2.1_0": "Verijātakaṁ tatiyaṁ."
}