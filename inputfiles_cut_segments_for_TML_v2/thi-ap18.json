{
  "thi-ap18:0.1_0": "Therīapadāna",
  "thi-ap18:0.2_0": "Ekūposathikavagga",
  "thi-ap18:0.3_0": "8. Khemātherīapadāna",
  "thi-ap18:1.1_0": "“Padumuttaro nāma jino,",
  "thi-ap18:1.2_0": "sabbadhammesu cakkhumā;",
  "thi-ap18:1.3_0": "Ito satasahassamhi,",
  "thi-ap18:1.4_0": "kappe uppajji nāyako.",
  "thi-ap18:2.1_0": "Tadāhaṁ haṁsavatiyaṁ,",
  "thi-ap18:2.2_0": "jātā seṭṭhikule ahuṁ;",
  "thi-ap18:2.3_0": "Nānāratanapajjote,",
  "thi-ap18:2.4_0": "mahāsukhasamappitā.",
  "thi-ap18:3.1_0": "Upetvā taṁ mahāvīraṁ,",
  "thi-ap18:3.2_0": "assosiṁ dhammadesanaṁ;",
  "thi-ap18:3.3_0": "Tato jātappasādāhaṁ,",
  "thi-ap18:3.4_0": "upemi saraṇaṁ jinaṁ.",
  "thi-ap18:4.1_0": "Mātaraṁ pitarañcāhaṁ,",
  "thi-ap18:4.2_0": "āyācitvā vināyakaṁ;",
  "thi-ap18:4.3_0": "Nimantayitvā sattāhaṁ,",
  "thi-ap18:4.4_0": "bhojayiṁ sahasāvakaṁ.",
  "thi-ap18:5.1_0": "Atikkante ca sattāhe,",
  "thi-ap18:5.2_0": "mahāpaññānamuttamaṁ;",
  "thi-ap18:5.3_0": "Bhikkhuniṁ etadaggamhi,",
  "thi-ap18:5.4_0": "ṭhapesi narasārathi.",
  "thi-ap18:6.1_0": "Taṁ sutvā muditā hutvā,",
  "thi-ap18:6.2_0": "puno tassa mahesino;",
  "thi-ap18:6.3_0": "Kāraṁ katvāna taṁ ṭhānaṁ,",
  "thi-ap18:6.4_0": "paṇipacca paṇīdahiṁ.",
  "thi-ap18:7.1_0": "Tato mama jino āha,",
  "thi-ap18:7.2_0": "‘sijjhataṁ paṇidhī tava;",
  "thi-ap18:7.3_0": "Sasaṅghe me kataṁ kāraṁ,",
  "thi-ap18:7.4_0": "appameyyaphalaṁ tayā.",
  "thi-ap18:8.1_0": "Satasahassito kappe,",
  "thi-ap18:8.2_0": "okkākakulasambhavo;",
  "thi-ap18:8.3_0": "Gotamo nāma gottena,",
  "thi-ap18:8.4_0": "satthā loke bhavissati.",
  "thi-ap18:9.1_0": "Tassa dhammesu dāyādā,",
  "thi-ap18:9.2_0": "orasā dhammanimmitā;",
  "thi-ap18:9.3_0": "Etadaggamanuppattā,",
  "thi-ap18:9.4_0": "khemā nāma bhavissati’.",
  "thi-ap18:10.1_0": "Tena kammena sukatena,",
  "thi-ap18:10.2_0": "cetanāpaṇidhīhi ca;",
  "thi-ap18:10.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "thi-ap18:10.4_0": "tāvatiṁsūpagā ahaṁ.",
  "thi-ap18:11.1_0": "Tato cutā yāmamagaṁ,",
  "thi-ap18:11.2_0": "tatohaṁ tusitaṁ gatā;",
  "thi-ap18:11.3_0": "Tato ca nimmānaratiṁ,",
  "thi-ap18:11.4_0": "vasavattipuraṁ tato.",
  "thi-ap18:12.1_0": "Yattha yatthūpapajjāmi,",
  "thi-ap18:12.2_0": "tassa kammassa vāhasā;",
  "thi-ap18:12.3_0": "Tattha tattheva rājūnaṁ,",
  "thi-ap18:12.4_0": "mahesittamakārayiṁ.",
  "thi-ap18:13.1_0": "Tato cutā manussatte,",
  "thi-ap18:13.2_0": "rājūnaṁ cakkavattinaṁ;",
  "thi-ap18:13.3_0": "Maṇḍalīnañca rājūnaṁ,",
  "thi-ap18:13.4_0": "mahesittamakārayiṁ.",
  "thi-ap18:14.1_0": "Sampattiṁ anubhotvāna,",
  "thi-ap18:14.2_0": "devesu manujesu ca;",
  "thi-ap18:14.3_0": "Sabbattha sukhitā hutvā,",
  "thi-ap18:14.4_0": "nekakappesu saṁsariṁ.",
  "thi-ap18:15.1_0": "Ekanavutito kappe,",
  "thi-ap18:15.2_0": "vipassī lokanāyako;",
  "thi-ap18:15.3_0": "Uppajji cārudassano,",
  "thi-ap18:15.4_0": "sabbadhammavipassako.",
  "thi-ap18:16.1_0": "Tamahaṁ lokanāyakaṁ,",
  "thi-ap18:16.2_0": "upetvā narasārathiṁ;",
  "thi-ap18:16.3_0": "Dhammaṁ bhaṇitaṁ sutvāna,",
  "thi-ap18:16.4_0": "pabbajiṁ anagāriyaṁ.",
  "thi-ap18:17.1_0": "Dasavassasahassāni,",
  "thi-ap18:17.2_0": "tassa vīrassa sāsane;",
  "thi-ap18:17.3_0": "Brahmacariyaṁ caritvāna,",
  "thi-ap18:17.4_0": "yuttayogā bahussutā.",
  "thi-ap18:18.1_0": "Paccayākārakusalā,",
  "thi-ap18:18.2_0": "catusaccavisāradā;",
  "thi-ap18:18.3_0": "Nipuṇā cittakathikā,",
  "thi-ap18:18.4_0": "satthusāsanakārikā.",
  "thi-ap18:19.1_0": "Tato cutāhaṁ tusitaṁ,",
  "thi-ap18:19.2_0": "upapannā yasassinī;",
  "thi-ap18:19.3_0": "Abhibhomi tahiṁ aññe,",
  "thi-ap18:19.4_0": "brahmacārīphalenahaṁ.",
  "thi-ap18:20.1_0": "Yattha yatthūpapannāhaṁ,",
  "thi-ap18:20.2_0": "mahābhogā mahaddhanā;",
  "thi-ap18:20.3_0": "Medhāvinī rūpavatī,",
  "thi-ap18:20.4_0": "vinītaparisāpi ca.",
  "thi-ap18:21.1_0": "Bhavāmi tena kammena,",
  "thi-ap18:21.2_0": "yogena jinasāsane;",
  "thi-ap18:21.3_0": "Sabbā sampattiyo mayhaṁ,",
  "thi-ap18:21.4_0": "sulabhā manaso piyā.",
  "thi-ap18:22.1_0": "Yopi me bhavate bhattā,",
  "thi-ap18:22.2_0": "yattha yattha gatāyapi;",
  "thi-ap18:22.3_0": "Vimāneti na maṁ koci,",
  "thi-ap18:22.4_0": "paṭipattibalena me.",
  "thi-ap18:23.1_0": "Imamhi bhaddake kappe,",
  "thi-ap18:23.2_0": "brahmabandhu mahāyaso;",
  "thi-ap18:23.3_0": "Nāmena koṇāgamano,",
  "thi-ap18:23.4_0": "uppajji vadataṁ varo.",
  "thi-ap18:24.1_0": "Tadā hi bārāṇasiyaṁ,",
  "thi-ap18:24.2_0": "susamiddhakulappajā;",
  "thi-ap18:24.3_0": "Dhanañjānī sumedhā ca,",
  "thi-ap18:24.4_0": "ahampi ca tayo janā.",
  "thi-ap18:25.1_0": "Saṅghārāmamadāsimha,",
  "thi-ap18:25.2_0": "dānasahāyikā pure;",
  "thi-ap18:25.3_0": "Saṅghassa ca vihārampi,",
  "thi-ap18:25.4_0": "uddissa kārikā mayaṁ.",
  "thi-ap18:26.1_0": "Tato cutā mayaṁ sabbā,",
  "thi-ap18:26.2_0": "tāvatiṁsūpagā ahuṁ;",
  "thi-ap18:26.3_0": "Yasasā aggataṁ pattā,",
  "thi-ap18:26.4_0": "manussesu tatheva ca.",
  "thi-ap18:27.1_0": "Imasmiṁyeva kappamhi,",
  "thi-ap18:27.2_0": "brahmabandhu mahāyaso;",
  "thi-ap18:27.3_0": "Kassapo nāma gottena,",
  "thi-ap18:27.4_0": "uppajji vadataṁ varo.",
  "thi-ap18:28.1_0": "Upaṭṭhāko mahesissa,",
  "thi-ap18:28.2_0": "tadā āsi narissaro;",
  "thi-ap18:28.3_0": "Kāsirājā kikī nāma,",
  "thi-ap18:28.4_0": "bārāṇasipuruttame.",
  "thi-ap18:29.1_0": "Tassāsiṁ jeṭṭhikā dhītā,",
  "thi-ap18:29.2_0": "samaṇī iti vissutā;",
  "thi-ap18:29.3_0": "Dhammaṁ sutvā jinaggassa,",
  "thi-ap18:29.4_0": "pabbajjaṁ samarocayiṁ.",
  "thi-ap18:30.1_0": "Anujāni na no tāto,",
  "thi-ap18:30.2_0": "agāreva tadā mayaṁ;",
  "thi-ap18:30.3_0": "Vīsavassasahassāni,",
  "thi-ap18:30.4_0": "vicarimha atanditā.",
  "thi-ap18:31.1_0": "Komāribrahmacariyaṁ,",
  "thi-ap18:31.2_0": "rājakaññā sukhedhitā;",
  "thi-ap18:31.3_0": "Buddhopaṭṭhānaniratā,",
  "thi-ap18:31.4_0": "muditā satta dhītaro.",
  "thi-ap18:32.1_0": "Samaṇī samaṇaguttā ca,",
  "thi-ap18:32.2_0": "bhikkhunī bhikkhudāyikā;",
  "thi-ap18:32.3_0": "Dhammā ceva sudhammā ca,",
  "thi-ap18:32.4_0": "sattamī saṅghadāyikā.",
  "thi-ap18:33.1_0": "Ahaṁ uppalavaṇṇā ca,",
  "thi-ap18:33.2_0": "paṭācārā ca kuṇḍalā;",
  "thi-ap18:33.3_0": "Kisāgotamī dhammadinnā,",
  "thi-ap18:33.4_0": "visākhā hoti sattamī.",
  "thi-ap18:34.1_0": "Kadāci so narādicco,",
  "thi-ap18:34.2_0": "dhammaṁ desesi abbhutaṁ;",
  "thi-ap18:34.3_0": "Mahānidānasuttantaṁ,",
  "thi-ap18:34.4_0": "sutvā taṁ pariyāpuṇiṁ.",
  "thi-ap18:35.1_0": "Tehi kammehi sukatehi,",
  "thi-ap18:35.2_0": "cetanāpaṇidhīhi ca;",
  "thi-ap18:35.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "thi-ap18:35.4_0": "tāvatiṁsamagacchahaṁ.",
  "thi-ap18:36.1_0": "Pacchime ca bhave dāni,",
  "thi-ap18:36.2_0": "sākalāya puruttame;",
  "thi-ap18:36.3_0": "Rañño maddassa dhītāmhi,",
  "thi-ap18:36.4_0": "manāpā dayitā piyā.",
  "thi-ap18:37.1_0": "Saha me jātamattamhi,",
  "thi-ap18:37.2_0": "khemaṁ tamhi pure ahu;",
  "thi-ap18:37.3_0": "Tato khemāti nāmaṁ me,",
  "thi-ap18:37.4_0": "guṇato upapajjatha.",
  "thi-ap18:38.1_0": "Yadāhaṁ yobbanaṁ pattā,",
  "thi-ap18:38.2_0": "rūpalāvaññabhūsitā;",
  "thi-ap18:38.3_0": "Tadā adāsi maṁ tāto,",
  "thi-ap18:38.4_0": "bimbisārassa rājino.",
  "thi-ap18:39.1_0": "Tassāhaṁ suppiyā āsiṁ,",
  "thi-ap18:39.2_0": "rūpake lāyane ratā;",
  "thi-ap18:39.3_0": "Rūpānaṁ dosavādīti,",
  "thi-ap18:39.4_0": "na upesiṁ mahādayaṁ.",
  "thi-ap18:40.1_0": "Bimbisāro tadā rājā,",
  "thi-ap18:40.2_0": "mamānuggahabuddhiyā;",
  "thi-ap18:40.3_0": "Vaṇṇayitvā veḷuvanaṁ,",
  "thi-ap18:40.4_0": "gāyake gāpayī mamaṁ.",
  "thi-ap18:41.1_0": "Rammaṁ veḷuvanaṁ yena,",
  "thi-ap18:41.2_0": "na diṭṭhaṁ sugatālayaṁ;",
  "thi-ap18:41.3_0": "Na tena nandanaṁ diṭṭhaṁ,",
  "thi-ap18:41.4_0": "iti maññāmase mayaṁ.",
  "thi-ap18:42.1_0": "Yena veḷuvanaṁ diṭṭhaṁ,",
  "thi-ap18:42.2_0": "naranandananandanaṁ;",
  "thi-ap18:42.3_0": "Sudiṭṭhaṁ nandanaṁ tena,",
  "thi-ap18:42.4_0": "amarindasunandanaṁ.",
  "thi-ap18:43.1_0": "Vihāya nandanaṁ devā,",
  "thi-ap18:43.2_0": "otaritvā mahītalaṁ;",
  "thi-ap18:43.3_0": "Rammaṁ veḷuvanaṁ disvā,",
  "thi-ap18:43.4_0": "na tappanti suvimhitā.",
  "thi-ap18:44.1_0": "Rājapuññena nibbattaṁ,",
  "thi-ap18:44.2_0": "buddhapuññena bhūsitaṁ;",
  "thi-ap18:44.3_0": "Ko vattā tassa nissesaṁ,",
  "thi-ap18:44.4_0": "vanassa guṇasañcayaṁ.",
  "thi-ap18:45.1_0": "Taṁ sutvā vanasamiddhaṁ,",
  "thi-ap18:45.2_0": "mama sotamanoharaṁ;",
  "thi-ap18:45.3_0": "Daṭṭhukāmā tamuyyānaṁ,",
  "thi-ap18:45.4_0": "rañño ārocayiṁ tadā.",
  "thi-ap18:46.1_0": "Mahatā parivārena,",
  "thi-ap18:46.2_0": "tadā ca so mahīpati;",
  "thi-ap18:46.3_0": "Maṁ pesesi tamuyyānaṁ,",
  "thi-ap18:46.4_0": "dassanāya samussukaṁ.",
  "thi-ap18:47.1_0": "‘Gaccha passa mahābhoge,",
  "thi-ap18:47.2_0": "vanaṁ nettarasāyanaṁ;",
  "thi-ap18:47.3_0": "Yaṁ sadā bhāti siriyā,",
  "thi-ap18:47.4_0": "sugatābhānurañjitaṁ’.",
  "thi-ap18:48.1_0": "‘Yadā ca piṇḍāya muni,",
  "thi-ap18:48.2_0": "giribbajapuruttamaṁ;",
  "thi-ap18:48.3_0": "Paviṭṭhohaṁ tadāyeva,",
  "thi-ap18:48.4_0": "vanaṁ daṭṭhumupāgamiṁ’.",
  "thi-ap18:49.1_0": "Tadā taṁ phullavipinaṁ,",
  "thi-ap18:49.2_0": "nānābhamarakūjitaṁ;",
  "thi-ap18:49.3_0": "Kokilāgītasahitaṁ,",
  "thi-ap18:49.4_0": "mayūragaṇanaccitaṁ.",
  "thi-ap18:50.1_0": "Appasaddamanākiṇṇaṁ,",
  "thi-ap18:50.2_0": "nānācaṅkamabhūsitaṁ;",
  "thi-ap18:50.3_0": "Kuṭimaṇḍapasaṅkiṇṇaṁ,",
  "thi-ap18:50.4_0": "yogīvaravirājitaṁ.",
  "thi-ap18:51.1_0": "Vicarantī amaññissaṁ,",
  "thi-ap18:51.2_0": "‘saphalaṁ nayanaṁ mama’;",
  "thi-ap18:51.3_0": "Tatthāpi taruṇaṁ bhikkhuṁ,",
  "thi-ap18:51.4_0": "yuttaṁ disvā vicintayiṁ.",
  "thi-ap18:52.1_0": "‘Īdise vipine ramme,",
  "thi-ap18:52.2_0": "ṭhitoyaṁ navayobbane;",
  "thi-ap18:52.3_0": "Vasantamiva kantena,",
  "thi-ap18:52.4_0": "rūpena ca samanvito.",
  "thi-ap18:53.1_0": "Nisinno rukkhamūlamhi,",
  "thi-ap18:53.2_0": "muṇḍo saṅghāṭipāruto;",
  "thi-ap18:53.3_0": "Jhāyate vatayaṁ bhikkhu,",
  "thi-ap18:53.4_0": "hitvā visayajaṁ ratiṁ.",
  "thi-ap18:54.1_0": "Nanu nāma gahaṭṭhena,",
  "thi-ap18:54.2_0": "kāmaṁ bhutvā yathāsukhaṁ;",
  "thi-ap18:54.3_0": "Pacchā jiṇṇena dhammoyaṁ,",
  "thi-ap18:54.4_0": "caritabbo subhaddako’.",
  "thi-ap18:55.1_0": "‘Suññakan’ti viditvāna,",
  "thi-ap18:55.2_0": "‘gandhagehaṁ jinālayaṁ’;",
  "thi-ap18:55.3_0": "Upetvā jinamaddakkhaṁ,",
  "thi-ap18:55.4_0": "udayantaṁva bhākaraṁ.",
  "thi-ap18:56.1_0": "Ekakaṁ sukhamāsīnaṁ,",
  "thi-ap18:56.2_0": "bījamānaṁ varitthiyā;",
  "thi-ap18:56.3_0": "Disvānevaṁ vicintesiṁ,",
  "thi-ap18:56.4_0": "‘nāyaṁ lūkho narāsabho.",
  "thi-ap18:57.1_0": "Sā kaññā kanakābhāsā,",
  "thi-ap18:57.2_0": "padumānanalocanā;",
  "thi-ap18:57.3_0": "Bimboṭṭhī kundadasanā,",
  "thi-ap18:57.4_0": "manonettarasāyanā.",
  "thi-ap18:58.1_0": "Hemadolābhasavanā,",
  "thi-ap18:58.2_0": "kalikākārasutthanī;",
  "thi-ap18:58.3_0": "Vedimajjhāva sussoṇī,",
  "thi-ap18:58.4_0": "rambhoru cārubhūsanā.",
  "thi-ap18:59.1_0": "Rattaṁsakupasambyānā,",
  "thi-ap18:59.2_0": "nīlamaṭṭhanivāsanā;",
  "thi-ap18:59.3_0": "Atappaneyyarūpena,",
  "thi-ap18:59.4_0": "hāsabhāvasamanvitā’.",
  "thi-ap18:60.1_0": "Disvā tamevaṁ cintesiṁ,",
  "thi-ap18:60.2_0": "‘ahoyamabhirūpinī;",
  "thi-ap18:60.3_0": "Na mayānena nettena,",
  "thi-ap18:60.4_0": "diṭṭhapubbā kudācanaṁ’.",
  "thi-ap18:61.1_0": "Tato jarābhibhūtā sā,",
  "thi-ap18:61.2_0": "vivaṇṇā vikatānanā;",
  "thi-ap18:61.3_0": "Bhinnadantā setasirā,",
  "thi-ap18:61.4_0": "salālā vadanāsuci.",
  "thi-ap18:62.1_0": "Saṅkhittakaṇṇā setakkhī,",
  "thi-ap18:62.2_0": "lambāsubhapayodharā;",
  "thi-ap18:62.3_0": "Valivitatasabbaṅgī,",
  "thi-ap18:62.4_0": "sirāvitatadehinī.",
  "thi-ap18:63.1_0": "Nataṅgā daṇḍadutiyā,",
  "thi-ap18:63.2_0": "upphāsulikatā kisā;",
  "thi-ap18:63.3_0": "Pavedhamānā patitā,",
  "thi-ap18:63.4_0": "nissasantī muhuṁ muhuṁ.",
  "thi-ap18:64.1_0": "Tato me āsi saṁvego,",
  "thi-ap18:64.2_0": "abbhuto lomahaṁsano;",
  "thi-ap18:64.3_0": "‘Dhiratthu rūpaṁ asuciṁ,",
  "thi-ap18:64.4_0": "ramante yattha bālisā’.",
  "thi-ap18:65.1_0": "Tadā mahākāruṇiko,",
  "thi-ap18:65.2_0": "disvā saṁviggamānasaṁ;",
  "thi-ap18:65.3_0": "Udaggacitto sugato,",
  "thi-ap18:65.4_0": "imā gāthā abhāsatha.",
  "thi-ap18:66.1_0": "‘Āturaṁ asuciṁ pūtiṁ,",
  "thi-ap18:66.2_0": "passa kheme samussayaṁ;",
  "thi-ap18:66.3_0": "Uggharantaṁ paggharantaṁ,",
  "thi-ap18:66.4_0": "bālānaṁ abhinanditaṁ.",
  "thi-ap18:67.1_0": "Asubhāya cittaṁ bhāvehi,",
  "thi-ap18:67.2_0": "ekaggaṁ susamāhitaṁ;",
  "thi-ap18:67.3_0": "Sati kāyagatā tyatthu,",
  "thi-ap18:67.4_0": "nibbidā bahulā bhava.",
  "thi-ap18:68.1_0": "Yathā idaṁ tathā etaṁ,",
  "thi-ap18:68.2_0": "yathā etaṁ tathā idaṁ;",
  "thi-ap18:68.3_0": "Ajjhattañca bahiddhā ca,",
  "thi-ap18:68.4_0": "kāye chandaṁ virājaya.",
  "thi-ap18:69.1_0": "Animittañca bhāvehi,",
  "thi-ap18:69.2_0": "mānānusayamujjaha;",
  "thi-ap18:69.3_0": "Tato mānābhisamayā,",
  "thi-ap18:69.4_0": "upasantā carissasi.",
  "thi-ap18:70.1_0": "Ye rāgarattānupatanti sotaṁ,",
  "thi-ap18:70.2_0": "Sayaṁ kataṁ makkaṭakova jālaṁ;",
  "thi-ap18:70.3_0": "Etampi chetvāna paribbajanti,",
  "thi-ap18:70.4_0": "Anapekkhino kāmasukhaṁ pahāya’.",
  "thi-ap18:71.1_0": "Tato kallitacittaṁ maṁ,",
  "thi-ap18:71.2_0": "ñatvāna narasārathi;",
  "thi-ap18:71.3_0": "Mahānidānaṁ desesi,",
  "thi-ap18:71.4_0": "suttantaṁ vinayāya me.",
  "thi-ap18:72.1_0": "Sutvā suttantaseṭṭhaṁ taṁ,",
  "thi-ap18:72.2_0": "pubbasaññamanussariṁ;",
  "thi-ap18:72.3_0": "Tattha ṭhitāvahaṁ santī,",
  "thi-ap18:72.4_0": "dhammacakkhuṁ visodhayiṁ.",
  "thi-ap18:73.1_0": "Nipatitvā mahesissa,",
  "thi-ap18:73.2_0": "pādamūlamhi tāvade;",
  "thi-ap18:73.3_0": "Accayaṁ desanatthāya,",
  "thi-ap18:73.4_0": "idaṁ vacanamabraviṁ.",
  "thi-ap18:74.1_0": "‘Namo te sabbadassāvī,",
  "thi-ap18:74.2_0": "namo te karuṇākara;",
  "thi-ap18:74.3_0": "Namo te tiṇṇasaṁsāra,",
  "thi-ap18:74.4_0": "namo te amataṁdada.",
  "thi-ap18:75.1_0": "Diṭṭhigahanapakkhandā,",
  "thi-ap18:75.2_0": "kāmarāgavimohitā;",
  "thi-ap18:75.3_0": "Tayā sammā upāyena,",
  "thi-ap18:75.4_0": "vinītā vinaye ratā.",
  "thi-ap18:76.1_0": "Adassanena vibhogā,",
  "thi-ap18:76.2_0": "tādisānaṁ mahesinaṁ;",
  "thi-ap18:76.3_0": "Anubhonti mahādukkhaṁ,",
  "thi-ap18:76.4_0": "sattā saṁsārasāgare.",
  "thi-ap18:77.1_0": "Yadāhaṁ lokasaraṇaṁ,",
  "thi-ap18:77.2_0": "araṇaṁ araṇantaguṁ;",
  "thi-ap18:77.3_0": "Nāddasāmi adūraṭṭhaṁ,",
  "thi-ap18:77.4_0": "desayāmi tamaccayaṁ.",
  "thi-ap18:78.1_0": "Mahāhitaṁ varadadaṁ,",
  "thi-ap18:78.2_0": "ahitoti visaṅkitā;",
  "thi-ap18:78.3_0": "Nopesiṁ rūpaniratā,",
  "thi-ap18:78.4_0": "desayāmi tamaccayaṁ’.",
  "thi-ap18:79.1_0": "Tadā madhuranigghoso,",
  "thi-ap18:79.2_0": "mahākāruṇiko jino;",
  "thi-ap18:79.3_0": "Avoca tiṭṭha khemeti,",
  "thi-ap18:79.4_0": "siñcanto amatena maṁ.",
  "thi-ap18:80.1_0": "Tadā paṇamya sirasā,",
  "thi-ap18:80.2_0": "katvā ca naṁ padakkhiṇaṁ;",
  "thi-ap18:80.3_0": "Gantvā disvā narapatiṁ,",
  "thi-ap18:80.4_0": "idaṁ vacanamabraviṁ.",
  "thi-ap18:81.1_0": "‘Aho sammā upāyo te,",
  "thi-ap18:81.2_0": "cintitoyamarindama;",
  "thi-ap18:81.3_0": "Vanadassanakāmāya,",
  "thi-ap18:81.4_0": "diṭṭho nibbānato muni.",
  "thi-ap18:82.1_0": "Yadi te ruccate rāja,",
  "thi-ap18:82.2_0": "sāsane tassa tādino;",
  "thi-ap18:82.3_0": "Pabbajissāmi rūpehaṁ,",
  "thi-ap18:82.4_0": "nibbinnā munivāṇinā’.",
  "thi-ap18:83.1_0": "Dutiyaṁ bhāṇavāraṁ.",
  "thi-ap18:84.1_0": "Añjaliṁ paggahetvāna,",
  "thi-ap18:84.2_0": "tadāha sa mahīpati;",
  "thi-ap18:84.3_0": "‘Anujānāmi te bhadde,",
  "thi-ap18:84.4_0": "pabbajjā tava sijjhatu’.",
  "thi-ap18:85.1_0": "Pabbajitvā tadā cāhaṁ,",
  "thi-ap18:85.2_0": "addhamāse upaṭṭhite;",
  "thi-ap18:85.3_0": "Dīpodayañca bhedañca,",
  "thi-ap18:85.4_0": "disvā saṁviggamānasā.",
  "thi-ap18:86.1_0": "Nibbinnā sabbasaṅkhāre,",
  "thi-ap18:86.2_0": "paccayākārakovidā;",
  "thi-ap18:86.3_0": "Caturoghe atikkamma,",
  "thi-ap18:86.4_0": "arahattamapāpuṇiṁ.",
  "thi-ap18:87.1_0": "Iddhīsu ca vasī āsiṁ,",
  "thi-ap18:87.2_0": "dibbāya sotadhātuyā;",
  "thi-ap18:87.3_0": "Cetopariyañāṇassa,",
  "thi-ap18:87.4_0": "vasī cāpi bhavāmahaṁ.",
  "thi-ap18:88.1_0": "Pubbenivāsaṁ jānāmi,",
  "thi-ap18:88.2_0": "dibbacakkhu visodhitaṁ;",
  "thi-ap18:88.3_0": "Sabbāsavā parikkhīṇā,",
  "thi-ap18:88.4_0": "natthi dāni punabbhavo.",
  "thi-ap18:89.1_0": "Atthadhammaniruttīsu,",
  "thi-ap18:89.2_0": "paṭibhāne tatheva ca;",
  "thi-ap18:89.3_0": "Parisuddhaṁ mama ñāṇaṁ,",
  "thi-ap18:89.4_0": "uppannaṁ buddhasāsane.",
  "thi-ap18:90.1_0": "Kusalāhaṁ visuddhīsu,",
  "thi-ap18:90.2_0": "kathāvatthuvisāradā;",
  "thi-ap18:90.3_0": "Abhidhammanayaññū ca,",
  "thi-ap18:90.4_0": "vasippattāmhi sāsane.",
  "thi-ap18:91.1_0": "Tato toraṇavatthusmiṁ,",
  "thi-ap18:91.2_0": "raññā kosalasāminā;",
  "thi-ap18:91.3_0": "Pucchitā nipuṇe pañhe,",
  "thi-ap18:91.4_0": "byākarontī yathātathaṁ.",
  "thi-ap18:92.1_0": "Tadā sa rājā sugataṁ,",
  "thi-ap18:92.2_0": "upasaṅkamma pucchatha;",
  "thi-ap18:92.3_0": "Tatheva buddho byākāsi,",
  "thi-ap18:92.4_0": "yathā te byākatā mayā.",
  "thi-ap18:93.1_0": "Jino tasmiṁ guṇe tuṭṭho,",
  "thi-ap18:93.2_0": "etadagge ṭhapesi maṁ;",
  "thi-ap18:93.3_0": "Mahāpaññānamaggāti,",
  "thi-ap18:93.4_0": "bhikkhunīnaṁ naruttamo.",
  "thi-ap18:94.1_0": "Kilesā jhāpitā mayhaṁ,",
  "thi-ap18:94.2_0": "…pe…",
  "thi-ap18:94.3_0": "viharāmi anāsavā.",
  "thi-ap18:95.1_0": "Svāgataṁ vata me āsi,",
  "thi-ap18:95.2_0": "…pe…",
  "thi-ap18:95.3_0": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap18:96.1_0": "Paṭisambhidā catasso,",
  "thi-ap18:96.2_0": "…pe…",
  "thi-ap18:96.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "thi-ap18:97.1_0": "Itthaṁ sudaṁ khemā bhikkhunī imā gāthāyo abhāsitthāti.",
  "thi-ap18:98.1_0": "Khemātheriyāpadānaṁ aṭṭhamaṁ."
}