{
  "ja300:0.1_0": "Jātaka",
  "ja300:0.2_0": "Tikanipāta",
  "ja300:0.3_0": "Kumbhavagga",
  "ja300:0.4_0": "10. Vakajātaka",
  "ja300:1.1_0": "“Parapāṇarodhā jīvanto,",
  "ja300:1.2_0": "maṁsalohitabhojano;",
  "ja300:1.3_0": "Vako vataṁ samādāya,",
  "ja300:1.4_0": "upapajji uposathaṁ.",
  "ja300:2.1_0": "Tassa sakko vataññāya,",
  "ja300:2.2_0": "ajarūpenupāgami;",
  "ja300:2.3_0": "Vītatapo ajjhappatto,",
  "ja300:2.4_0": "bhañji lohitapo tapaṁ.",
  "ja300:3.1_0": "Evameva idhekacce,",
  "ja300:3.2_0": "Samādānamhi dubbalā;",
  "ja300:3.3_0": "Lahuṁ karonti attānaṁ,",
  "ja300:3.4_0": "Vakova ajakāraṇā”ti.",
  "ja300:4.1_0": "Vakajātakaṁ dasamaṁ.",
  "ja300:4.2_0": "Kumbhavaggo pañcamo.",
  "ja300:5.1_0": "Tassuddānaṁ",
  "ja300:6.1_0": "Varakumbha supattasirivhayano,",
  "ja300:6.2_0": "Sucisammata bindusaro cusabho;",
  "ja300:6.3_0": "Saritaṁpati caṇḍi jarākapinā,",
  "ja300:6.4_0": "Atha makkaṭiyā vakakena dasāti.",
  "ja300:7.1_0": "Atha vagguddānaṁ",
  "ja300:8.1_0": "Saṅkappo padumo ceva,",
  "ja300:8.2_0": "Udapānena tatiyaṁ;",
  "ja300:8.3_0": "Abbhantaraṁ ghaṭabhedaṁ,",
  "ja300:8.4_0": "Tikanipātamhilaṅkatanti.",
  "ja300:9.1_0": "Tikanipātaṁ niṭṭhitaṁ."
}