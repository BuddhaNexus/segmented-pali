{
  "patthana12.16:0.1_0": "Paṭṭhānapakaraṇa",
  "patthana12.16:0.2_0": "Dhammapaccanīya (2), Dukadukapaṭṭhānapāḷi (6)",
  "patthana12.16:0.3_0": "12.16. Saraṇaduka, Āsavagocchaka",
  "patthana12.16:1.1_0": "Nasaraṇaṁ noāsavaṁ dhammaṁ paṭicca ….",
  "patthana12.16:1.1_1": "(Saṅkhittaṁ.) Hetuyā pañca, ārammaṇe dve …pe…",
  "patthana12.16:1.1_2": "vipāke ekaṁ …pe…",
  "patthana12.16:1.1_3": "avigate pañca.",
  "patthana12.16:2.1_0": "Nasaraṇaṁ nanoāsavaṁ dhammaṁ paṭicca ….",
  "patthana12.16:3.1_0": "Nasaraṇaṁ nasāsavaṁ dhammaṁ paṭicca ….",
  "patthana12.16:4.1_0": "Nasaraṇaṁ naanāsavaṁ dhammaṁ paṭicca ….",
  "patthana12.16:4.1_1": "(Saṅkhittaṁ.) Hetuyā pañca, ārammaṇe dve …pe…",
  "patthana12.16:4.1_2": "vipāke ekaṁ …pe…",
  "patthana12.16:4.1_3": "avigate pañca.",
  "patthana12.16:5.1_0": "Nasaraṇaṁ naāsavasampayuttaṁ dhammaṁ paṭicca ….",
  "patthana12.16:5.1_1": "(Saṅkhittaṁ.) Hetuyā tīṇi, ārammaṇe ekaṁ …pe…",
  "patthana12.16:5.1_2": "avigate tīṇi.",
  "patthana12.16:6.1_0": "Nasaraṇaṁ naāsavavippayuttaṁ dhammaṁ paṭicca ….",
  "patthana12.16:7.1_0": "Nasaraṇaṁ naāsavañceva naanāsavañca dhammaṁ paṭicca ….",
  "patthana12.16:7.1_1": "(Saṅkhittaṁ.) Hetuyā pañca, ārammaṇe dve …pe…",
  "patthana12.16:7.1_2": "vipāke ekaṁ …pe…",
  "patthana12.16:7.1_3": "avigate pañca.",
  "patthana12.16:8.1_0": "Nasaraṇaṁ naanāsavañceva nano ca āsavaṁ dhammaṁ paṭicca ….",
  "patthana12.16:9.1_0": "Nasaraṇaṁ naāsavañceva naāsavavippayuttañca dhammaṁ paṭicca …pe…",
  "patthana12.16:9.1_1": "nasaraṇaṁ naāsavavippayuttañceva nano ca āsavaṁ dhammaṁ paṭicca ….",
  "patthana12.16:10.1_0": "Nasaraṇaṁ naāsavavippayuttaṁ nasāsavaṁ dhammaṁ paṭicca ….",
  "patthana12.16:11.1_0": "Nasaraṇaṁ naāsavavippayuttaṁ naanāsavaṁ dhammaṁ paṭicca ….",
  "patthana12.16:11.1_1": "(Saṅkhittaṁ.) Hetuyā tīṇi, ārammaṇe ekaṁ …pe…",
  "patthana12.16:11.1_2": "avigate tīṇi."
}