{
  "ja172:0.1_0": "Jātaka",
  "ja172:0.2_0": "Dukanipāta",
  "ja172:0.3_0": "Kalyāṇavagga",
  "ja172:0.4_0": "2. Daddarajātaka",
  "ja172:1.1_0": "“Ko nu saddena mahatā,",
  "ja172:1.2_0": "abhinādeti daddaraṁ;",
  "ja172:1.3_0": "Taṁ sīhā nappaṭinadanti,",
  "ja172:1.4_0": "ko nāmeso migādhibhū”.",
  "ja172:2.1_0": "“Adhamo migajātānaṁ,",
  "ja172:2.2_0": "siṅgālo tāta vassati;",
  "ja172:2.3_0": "Jātimassa jigucchantā,",
  "ja172:2.4_0": "tuṇhī sīhā samacchare”ti.",
  "ja172:3.1_0": "Daddarajātakaṁ dutiyaṁ."
}