{
  "ja259:0.1_0": "Jātaka",
  "ja259:0.2_0": "Tikanipāta",
  "ja259:0.3_0": "Saṅkappavagga",
  "ja259:0.4_0": "9. Tirīṭavacchajātaka",
  "ja259:1.1_0": "“Nayimassa vijjāmayamatthi kiñci,",
  "ja259:1.2_0": "Na bandhavo no pana te sahāyo;",
  "ja259:1.3_0": "Atha kena vaṇṇena tirīṭavaccho,",
  "ja259:1.4_0": "Tedaṇḍiko bhuñjati aggapiṇḍaṁ”.",
  "ja259:2.1_0": "“Āpāsu me yuddhaparājitassa,",
  "ja259:2.2_0": "Ekassa katvā vivanasmi ghore;",
  "ja259:2.3_0": "Pasārayī kicchagatassa pāṇiṁ,",
  "ja259:2.4_0": "Tenūdatāriṁ dukhasampareto.",
  "ja259:3.1_0": "Etassa kiccena idhānupatto,",
  "ja259:3.2_0": "Vesāyino visayā jīvaloke;",
  "ja259:3.3_0": "Lābhāraho tāta tirīṭavaccho,",
  "ja259:3.4_0": "Dethassa bhogaṁ yajathañca yaññan”ti.",
  "ja259:4.1_0": "Tirīṭavacchajātakaṁ navamaṁ."
}