{
  "tha-ap555:0.1_0": "Therāpadāna",
  "tha-ap555:0.2_0": "Yasavagga",
  "tha-ap555:0.3_0": "3. Gayākassapattheraapadāna",
  "tha-ap555:1.1_0": "“Ajinacammavatthohaṁ,",
  "tha-ap555:1.2_0": "khāribhāradharo tadā;",
  "tha-ap555:1.3_0": "Khārikaṁ hārayitvāna,",
  "tha-ap555:1.4_0": "kolaṁ ahāsi assamaṁ.",
  "tha-ap555:2.1_0": "Bhagavā tamhi samaye,",
  "tha-ap555:2.2_0": "eko adutiyo jino;",
  "tha-ap555:2.3_0": "Mamassamaṁ upāgacchi,",
  "tha-ap555:2.4_0": "jotento sabbakālikaṁ.",
  "tha-ap555:3.1_0": "Sakaṁ cittaṁ pasādetvā,",
  "tha-ap555:3.2_0": "abhivādetvāna subbataṁ;",
  "tha-ap555:3.3_0": "Ubho hatthehi paggayha,",
  "tha-ap555:3.4_0": "kolaṁ buddhassadāsahaṁ.",
  "tha-ap555:4.1_0": "Ekatiṁse ito kappe,",
  "tha-ap555:4.2_0": "yaṁ phalaṁ adadiṁ tadā;",
  "tha-ap555:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap555:4.4_0": "koladānassidaṁ phalaṁ.",
  "tha-ap555:5.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap555:5.2_0": "…pe…",
  "tha-ap555:5.3_0": "viharāmi anāsavo.",
  "tha-ap555:6.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap555:6.2_0": "…pe…",
  "tha-ap555:6.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap555:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap555:7.2_0": "…pe…",
  "tha-ap555:7.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap555:8.1_0": "Itthaṁ sudaṁ āyasmā gayākassapo thero imā gāthāyo abhāsitthāti.",
  "tha-ap555:9.1_0": "Gayākassapattherassāpadānaṁ tatiyaṁ."
}