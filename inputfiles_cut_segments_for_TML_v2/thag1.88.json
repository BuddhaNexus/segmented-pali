{
  "thag1.88:0.1_0": "Theragāthā",
  "thag1.88:0.2_0": "Ekakanipāta",
  "thag1.88:0.3_0": "Navamavagga",
  "thag1.88:0.4_0": "1.88. Ajjunattheragāthā",
  "thag1.88:1.1_0": "“Asakkhiṁ vata attānaṁ,",
  "thag1.88:1.2_0": "uddhātuṁ udakā thalaṁ;",
  "thag1.88:1.3_0": "Vuyhamāno mahogheva,",
  "thag1.88:1.4_0": "saccāni paṭivijjhahan”ti.",
  "thag1.88:1.5_0": "… Ajjuno thero …."
}