{
  "ja128:0.1_0": "Jātaka",
  "ja128:0.2_0": "Ekakanipāta",
  "ja128:0.3_0": "Kusanāḷivagga",
  "ja128:0.4_0": "8. Biḷāravatajātaka",
  "ja128:1.1_0": "“Yo ve dhammaṁ dhajaṁ katvā,",
  "ja128:1.2_0": "Nigūḷho pāpamācare;",
  "ja128:1.3_0": "Vissāsayitvā bhūtāni,",
  "ja128:1.4_0": "Biḷāraṁ nāma taṁ vatan”ti.",
  "ja128:2.1_0": "Biḷāravatajātakaṁ aṭṭhamaṁ."
}