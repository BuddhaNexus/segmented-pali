{
  "ja351:0.1_0": "Jātaka",
  "ja351:0.2_0": "Pañcakanipāta",
  "ja351:0.3_0": "Maṇikuṇḍalavagga",
  "ja351:0.4_0": "1. Maṇikuṇḍalajātaka",
  "ja351:1.1_0": "“Jīno rathassaṁ maṇikuṇḍale ca,",
  "ja351:1.2_0": "Putte ca dāre ca tatheva jīno;",
  "ja351:1.3_0": "Sabbesu bhogesu asesakesu,",
  "ja351:1.4_0": "Kasmā na santappasi sokakāle”.",
  "ja351:2.1_0": "“Pubbeva maccaṁ vijahanti bhogā,",
  "ja351:2.2_0": "Macco vā te pubbataraṁ jahāti;",
  "ja351:2.3_0": "Asassatā bhogino kāmakāmi,",
  "ja351:2.4_0": "Tasmā na socāmahaṁ sokakāle.",
  "ja351:3.1_0": "Udeti āpūrati veti cando,",
  "ja351:3.2_0": "Atthaṁ tapetvāna paleti sūriyo;",
  "ja351:3.3_0": "Viditā mayā sattuka lokadhammā,",
  "ja351:3.4_0": "Tasmā na socāmahaṁ sokakāle”.",
  "ja351:4.1_0": "“Alaso gihī kāmabhogī na sādhu,",
  "ja351:4.2_0": "Asaññato pabbajito na sādhu;",
  "ja351:4.3_0": "Rājā na sādhu anisammakārī,",
  "ja351:4.4_0": "Yo paṇḍito kodhano taṁ na sādhu.",
  "ja351:5.1_0": "Nisamma khattiyo kayirā,",
  "ja351:5.2_0": "nānisamma disampati;",
  "ja351:5.3_0": "Nisammakārino rāja,",
  "ja351:5.4_0": "yaso kitti ca vaḍḍhatī”ti.",
  "ja351:6.1_0": "Maṇikuṇḍalajātakaṁ paṭhamaṁ."
}