{
  "tha-ap326:0.1_0": "Therāpadāna",
  "tha-ap326:0.2_0": "Umāpupphiyavagga",
  "tha-ap326:0.3_0": "4. Yaññasāmikattheraapadāna",
  "tha-ap326:1.1_0": "“Jātiyā sattavassohaṁ,",
  "tha-ap326:1.2_0": "ahosiṁ mantapāragū;",
  "tha-ap326:1.3_0": "Kulavattaṁ adhāresiṁ,",
  "tha-ap326:1.4_0": "yañño ussāhito mayā.",
  "tha-ap326:2.1_0": "Cullāsītisahassāni,",
  "tha-ap326:2.2_0": "pasū haññanti me tadā;",
  "tha-ap326:2.3_0": "Sārathambhupanītāni,",
  "tha-ap326:2.4_0": "yaññatthāya upaṭṭhitā.",
  "tha-ap326:3.1_0": "Ukkāmukhapahaṭṭhova,",
  "tha-ap326:3.2_0": "khadiraṅgārasannibho;",
  "tha-ap326:3.3_0": "Udayantova sūriyo,",
  "tha-ap326:3.4_0": "puṇṇamāyeva candimā.",
  "tha-ap326:4.1_0": "Siddhattho sabbasiddhattho,",
  "tha-ap326:4.2_0": "tilokamahito hito;",
  "tha-ap326:4.3_0": "Upagantvāna sambuddho,",
  "tha-ap326:4.4_0": "idaṁ vacanamabravi.",
  "tha-ap326:5.1_0": "‘Ahiṁsā sabbapāṇīnaṁ,",
  "tha-ap326:5.2_0": "kumāra mama ruccati;",
  "tha-ap326:5.3_0": "Theyyā ca aticārā ca,",
  "tha-ap326:5.4_0": "majjapānā ca ārati.",
  "tha-ap326:6.1_0": "Rati ca samacariyāya,",
  "tha-ap326:6.2_0": "bāhusaccaṁ kataññutā;",
  "tha-ap326:6.3_0": "Diṭṭhe dhamme parattha ca,",
  "tha-ap326:6.4_0": "dhammā ete pasaṁsiyā.",
  "tha-ap326:7.1_0": "Ete dhamme bhāvayitvā,",
  "tha-ap326:7.2_0": "sabbasattahite rato;",
  "tha-ap326:7.3_0": "Buddhe cittaṁ pasādetvā,",
  "tha-ap326:7.4_0": "bhāvehi maggamuttamaṁ’.",
  "tha-ap326:8.1_0": "Idaṁ vatvāna sabbaññū,",
  "tha-ap326:8.2_0": "lokajeṭṭho narāsabho;",
  "tha-ap326:8.3_0": "Mamevaṁ anusāsitvā,",
  "tha-ap326:8.4_0": "vehāsaṁ uggato gato.",
  "tha-ap326:9.1_0": "Pubbe cittaṁ visodhetvā,",
  "tha-ap326:9.2_0": "pacchā cittaṁ pasādayiṁ;",
  "tha-ap326:9.3_0": "Tena cittappasādena,",
  "tha-ap326:9.4_0": "tusitaṁ upapajjahaṁ.",
  "tha-ap326:10.1_0": "Catunnavutito kappe,",
  "tha-ap326:10.2_0": "yadā cittaṁ pasādayiṁ;",
  "tha-ap326:10.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap326:10.4_0": "buddhasaññāyidaṁ phalaṁ.",
  "tha-ap326:11.1_0": "Paṭisambhidā catasso,",
  "tha-ap326:11.2_0": "…pe…",
  "tha-ap326:11.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap326:12.1_0": "Itthaṁ sudaṁ āyasmā yaññasāmiko thero imā gāthāyo abhāsitthāti.",
  "tha-ap326:13.1_0": "Yaññasāmikattherassāpadānaṁ catutthaṁ."
}