{
  "ja454:0.1_0": "Jātaka",
  "ja454:0.2_0": "Dasakanipāta",
  "ja454:0.3_0": "Catudvāravagga",
  "ja454:0.4_0": "16. Ghaṭapaṇḍitajātaka",
  "ja454:1.1_0": "“Uṭṭhehi kaṇha kiṁ sesi,",
  "ja454:1.2_0": "Ko attho supanena te;",
  "ja454:1.3_0": "Yopi tuyhaṁ sako bhātā,",
  "ja454:1.4_0": "Hadayaṁ cakkhu ca dakkhiṇaṁ;",
  "ja454:1.5_0": "Tassa vātā balīyanti,",
  "ja454:1.6_0": "Ghaṭo jappati kesava”.",
  "ja454:2.1_0": "“Tassa taṁ vacanaṁ sutvā,",
  "ja454:2.2_0": "rohiṇeyyassa kesavo;",
  "ja454:2.3_0": "Taramānarūpo vuṭṭhāsi,",
  "ja454:2.4_0": "bhātusokena aṭṭito”.",
  "ja454:3.1_0": "“Kiṁ nu ummattarūpova,",
  "ja454:3.2_0": "kevalaṁ dvārakaṁ imaṁ;",
  "ja454:3.3_0": "Saso sasoti lapasi,",
  "ja454:3.4_0": "ko nu te sasamāhari”.",
  "ja454:4.1_0": "“Sovaṇṇamayaṁ maṇīmayaṁ,",
  "ja454:4.2_0": "Lohamayaṁ atha rūpiyāmayaṁ;",
  "ja454:4.3_0": "Saṅkhasilāpavāḷamayaṁ,",
  "ja454:4.4_0": "Kārayissāmi te sasaṁ”.",
  "ja454:5.1_0": "“Santi aññepi sasakā,",
  "ja454:5.2_0": "araññe vanagocarā;",
  "ja454:5.3_0": "Tepi te ānayissāmi,",
  "ja454:5.4_0": "kīdisaṁ sasamicchasi”.",
  "ja454:6.1_0": "“Na cāhamete icchāmi,",
  "ja454:6.2_0": "ye sasā pathavissitā;",
  "ja454:6.3_0": "Candato sasamicchāmi,",
  "ja454:6.4_0": "taṁ me ohara kesava”.",
  "ja454:7.1_0": "“So nūna madhuraṁ ñāti,",
  "ja454:7.2_0": "jīvitaṁ vijahissasi;",
  "ja454:7.3_0": "Apatthiyaṁ yo patthayasi,",
  "ja454:7.4_0": "candato sasamicchasi”.",
  "ja454:8.1_0": "“Evañce kaṇha jānāsi,",
  "ja454:8.2_0": "yadaññamanusāsasi;",
  "ja454:8.3_0": "Kasmā pure mataṁ puttaṁ,",
  "ja454:8.4_0": "ajjāpi manusocasi”.",
  "ja454:9.1_0": "“Yaṁ na labbhā manussena,",
  "ja454:9.2_0": "amanussena vā puna;",
  "ja454:9.3_0": "Jāto me mā marī putto,",
  "ja454:9.4_0": "kuto labbhā alabbhiyaṁ.",
  "ja454:10.1_0": "Na mantā mūlabhesajjā,",
  "ja454:10.2_0": "osadhehi dhanena vā;",
  "ja454:10.3_0": "Sakkā ānayituṁ kaṇha,",
  "ja454:10.4_0": "yaṁ petamanusocasi”.",
  "ja454:11.1_0": "“Yassa etādisā assu,",
  "ja454:11.2_0": "amaccā purisapaṇḍitā;",
  "ja454:11.3_0": "Yathā nijjhāpaye ajja,",
  "ja454:11.4_0": "ghaṭo purisapaṇḍito.",
  "ja454:12.1_0": "Ādittaṁ vata maṁ santaṁ,",
  "ja454:12.2_0": "ghatasittaṁva pāvakaṁ;",
  "ja454:12.3_0": "Vārinā viya osiñcaṁ,",
  "ja454:12.4_0": "sabbaṁ nibbāpaye daraṁ.",
  "ja454:13.1_0": "Abbahī vata me sallaṁ,",
  "ja454:13.2_0": "yamāsi hadayassitaṁ;",
  "ja454:13.3_0": "Yo me sokaparetassa,",
  "ja454:13.4_0": "puttasokaṁ apānudi.",
  "ja454:14.1_0": "Sohaṁ abbūḷhasallosmi,",
  "ja454:14.2_0": "vītasoko anāvilo;",
  "ja454:14.3_0": "Na socāmi na rodāmi,",
  "ja454:14.4_0": "tava sutvāna māṇava”.",
  "ja454:15.1_0": "“Evaṁ karonti sappaññā,",
  "ja454:15.2_0": "Ye honti anukampakā;",
  "ja454:15.3_0": "Nivattayanti sokamhā,",
  "ja454:15.4_0": "Ghaṭo jeṭṭhaṁva bhātaran”ti.",
  "ja454:16.1_0": "Ghaṭapaṇḍitajātakaṁ soḷasamaṁ.",
  "ja454:17.1_0": "Dasakanipātaṁ niṭṭhitaṁ.",
  "ja454:18.1_0": "Tassuddānaṁ",
  "ja454:19.1_0": "Daḷha kaṇha dhanañjaya saṅkhavaro,",
  "ja454:19.2_0": "Raja sattaha kassa ca takkalinā;",
  "ja454:19.3_0": "Dhammaṁ kukkuṭa kuṇḍali bhojanadā,",
  "ja454:19.4_0": "Cakkavāka subhūrisa sotthi ghaṭoti."
}