{
  "anya-e0603n0:0_0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e0603n0:1_0": "Lakkhaṇāto",
  "anya-e0603n0:2_0": "Buddhathomanāgāthā",
  "anya-e0603n0:3_0": "1.",
  "anya-e0603n0:4_0": "Bhavābhavesu nekesu,",
  "anya-e0603n0:5_0": "Pūresi tiṁsapāramī;",
  "anya-e0603n0:6_0": "Cariyāyoca sambuddho,",
  "anya-e0603n0:7_0": "Pariccāgejahaṁsadā.",
  "anya-e0603n0:8_0": "2.",
  "anya-e0603n0:9_0": "Vessantarattabhāvamhi,",
  "anya-e0603n0:10_0": "Ṭhitomaddiṁ pisoadā;",
  "anya-e0603n0:11_0": "Nijaṁkaṇhājinaṁjāliṁ,",
  "anya-e0603n0:12_0": "Viyaṁvaṅkatapabbate.",
  "anya-e0603n0:13_0": "3.",
  "anya-e0603n0:14_0": "Tatocavitvātusite,",
  "anya-e0603n0:15_0": "Pureuppajjipuññavā;",
  "anya-e0603n0:16_0": "Rūpādidasaṭhānehi,",
  "anya-e0603n0:17_0": "Aññedeveatikkami.",
  "anya-e0603n0:18_0": "4.",
  "anya-e0603n0:19_0": "Cakkavāḷasahassehi,",
  "anya-e0603n0:20_0": "Dasahāgammasabbaso;",
  "anya-e0603n0:21_0": "Yācitodevabrahmūhi,",
  "anya-e0603n0:22_0": "Buddhabhāvamanonato.",
  "anya-e0603n0:23_0": "5.",
  "anya-e0603n0:24_0": "Vilokanānisopañca,",
  "anya-e0603n0:25_0": "Viloketvāsirindharo;",
  "anya-e0603n0:26_0": "Tatocavitvāmāyāya,",
  "anya-e0603n0:27_0": "Uppajjisākiyekule.",
  "anya-e0603n0:28_0": "6.",
  "anya-e0603n0:29_0": "Visākhapuṇṇameseṭṭhe,",
  "anya-e0603n0:30_0": "Vijāyilumbinīvane;",
  "anya-e0603n0:31_0": "Sīhanādaṁpanādento,",
  "anya-e0603n0:32_0": "Sattapādaṁgatotadā.",
  "anya-e0603n0:33_0": "8.",
  "anya-e0603n0:34_0": "Virociṁsutadākāse,",
  "anya-e0603n0:35_0": "Tārocandadivākarā;",
  "anya-e0603n0:36_0": "Ratanānicabhūmaṭṭhā,",
  "anya-e0603n0:37_0": "Nānāpupphācapupphare.",
  "anya-e0603n0:38_0": "8.",
  "anya-e0603n0:39_0": "Sītāyanti avīcaggī,",
  "anya-e0603n0:40_0": "Khuppipāsācasohitā;",
  "anya-e0603n0:41_0": "Lokantaremahāpabhā,",
  "anya-e0603n0:42_0": "Andhāpassantitāvade.",
  "anya-e0603n0:43_0": "9.",
  "anya-e0603n0:44_0": "Aghaṭṭitāvāditāni,",
  "anya-e0603n0:45_0": "Tūriyānicaambare;",
  "anya-e0603n0:46_0": "Madhurenapanādiṁsu,",
  "anya-e0603n0:47_0": "Yasaṁsarenatāvade.",
  "anya-e0603n0:48_0": "10.",
  "anya-e0603n0:49_0": "Thutigītānigāyantā,",
  "anya-e0603n0:50_0": "Devācakamalāsanā;",
  "anya-e0603n0:51_0": "Sammodentāvaaṭṭhaṁsu,",
  "anya-e0603n0:52_0": "Buddhaṅkurassasantike.",
  "anya-e0603n0:53_0": "11.",
  "anya-e0603n0:54_0": "Kumārocārudhātīhi,",
  "anya-e0603n0:55_0": "Tositārājamandire;",
  "anya-e0603n0:56_0": "Vuddhosisusukhaṁkhemaṁ,",
  "anya-e0603n0:57_0": "Sakyakaññānamantare.",
  "anya-e0603n0:58_0": "12.",
  "anya-e0603n0:59_0": "Soḷasavassikosakya,",
  "anya-e0603n0:60_0": "Nandanopitarāsatā;",
  "anya-e0603n0:61_0": "Rājatteabhisittosi,",
  "anya-e0603n0:62_0": "Modesituvanattaye.",
  "anya-e0603n0:63_0": "13.",
  "anya-e0603n0:64_0": "Nandanānaṁsovaṇṇa,",
  "anya-e0603n0:65_0": "Vaṇṇasobhaṁyasodharaṁ;",
  "anya-e0603n0:66_0": "Sokāresimahesittaṁ,",
  "anya-e0603n0:67_0": "Lokanettarasāyanaṁ.",
  "anya-e0603n0:68_0": "14.",
  "anya-e0603n0:69_0": "Ekūnatiṁsamevasse,",
  "anya-e0603n0:70_0": "Visākhapuṇṇamevare;",
  "anya-e0603n0:71_0": "Sahasenāyauyyāsi,",
  "anya-e0603n0:72_0": "Yānenuyyānamaṇḍalaṁ.",
  "anya-e0603n0:73_0": "15.",
  "anya-e0603n0:74_0": "Nimittecaturodisvā,",
  "anya-e0603n0:75_0": "Puttassacavijāyanaṁ;",
  "anya-e0603n0:76_0": "Sutvāubbiggacittena,",
  "anya-e0603n0:77_0": "Naramitthapuretahiṁ.",
  "anya-e0603n0:78_0": "16.",
  "anya-e0603n0:79_0": "Paccāgammapuraṁtamhā,",
  "anya-e0603n0:80_0": "Pattopāsādamaṇḍalaṁ;",
  "anya-e0603n0:81_0": "Nippajjidevarājāva,",
  "anya-e0603n0:82_0": "Sayanesomahārahe.",
  "anya-e0603n0:83_0": "17.",
  "anya-e0603n0:84_0": "Sundarītaṁpurakkhitvā,",
  "anya-e0603n0:85_0": "Naccagītenatosayuṁ;",
  "anya-e0603n0:86_0": "Evaṁpisonaramitvā,",
  "anya-e0603n0:87_0": "Āhunikkhamamānaso.",
  "anya-e0603n0:88_0": "18.",
  "anya-e0603n0:89_0": "Pathamaṁrāhulaṁputtaṁ,",
  "anya-e0603n0:90_0": "Passitvānikkhamāmahaṁ;",
  "anya-e0603n0:91_0": "Iticintiyamāneso,",
  "anya-e0603n0:92_0": "Agājāyānivesanaṁ;",
  "anya-e0603n0:93_0": "19.",
  "anya-e0603n0:94_0": "Thapetvāpādadummāre,",
  "anya-e0603n0:95_0": "Gīvamantopavesayaṁ;",
  "anya-e0603n0:96_0": "Olokayaṁsapassittha,",
  "anya-e0603n0:97_0": "Nippannaṁtaṁsamātaraṁ.",
  "anya-e0603n0:98_0": "20.",
  "anya-e0603n0:99_0": "Gaṇheyyaṁtaṁapanetvā,",
  "anya-e0603n0:100_0": "Bāhuṁcedeviyāahaṁ;",
  "anya-e0603n0:101_0": "Nadadenikkhamokāsaṁ,",
  "anya-e0603n0:102_0": "Pabujjhitvāyasodharā.",
  "anya-e0603n0:103_0": "21.",
  "anya-e0603n0:104_0": "Buddhapattovapasseyyaṁ,",
  "anya-e0603n0:105_0": "Rāhulaṁiticintiya;",
  "anya-e0603n0:106_0": "Kaṇḍakaṁāruhitvāna,",
  "anya-e0603n0:107_0": "Tamhāchannenanikkhami.",
  "anya-e0603n0:108_0": "22.",
  "anya-e0603n0:109_0": "Dvārāsayāvivariṁsu,",
  "anya-e0603n0:110_0": "Devatādvāramaṇḍalaṁ;",
  "anya-e0603n0:111_0": "Ratanukkāsahassāni,",
  "anya-e0603n0:112_0": "Jālayiṁsumarūtahiṁ.",
  "anya-e0603n0:113_0": "23.",
  "anya-e0603n0:114_0": "Mānikkhamassusiddhattha,",
  "anya-e0603n0:115_0": "Itotvaṁsattamedine;",
  "anya-e0603n0:116_0": "Bhavissasecakkavattī,",
  "anya-e0603n0:117_0": "Itinīvāripāpimā.",
  "anya-e0603n0:118_0": "24.",
  "anya-e0603n0:119_0": "Tagghajānāmitaṁmāra,",
  "anya-e0603n0:120_0": "Idhamātiṭṭhapakkama;",
  "anya-e0603n0:121_0": "Itimāraṁ palāpetvā,",
  "anya-e0603n0:122_0": "Vissaṭṭhosoabhikkami.",
  "anya-e0603n0:123_0": "25.",
  "anya-e0603n0:124_0": "Patvānomanadītīraṁ,",
  "anya-e0603n0:125_0": "Punāgamesituragaṁ;",
  "anya-e0603n0:126_0": "Channañcanagaraṁ bimbā;",
  "anya-e0603n0:127_0": "Devilāpaṭivedituṁ.",
  "anya-e0603n0:128_0": "26.",
  "anya-e0603n0:129_0": "Bandhitaṁyasodharāya,",
  "anya-e0603n0:130_0": "Moḷiṁsugandhavāsitaṁ;",
  "anya-e0603n0:131_0": "Chetvākhaggenatiṇhena,",
  "anya-e0603n0:132_0": "Ukkhipitthatamambare.",
  "anya-e0603n0:133_0": "27.",
  "anya-e0603n0:134_0": "Aṭṭhaseṭṭhaparikkhāre,",
  "anya-e0603n0:135_0": "Dhāretvābrahmunābhate;",
  "anya-e0603n0:136_0": "Hitvārājārahaṁdussaṁ,",
  "anya-e0603n0:137_0": "Samaṇatthamupāgami.",
  "anya-e0603n0:138_0": "28.",
  "anya-e0603n0:139_0": "Karontodukkaraṁkammaṁ,",
  "anya-e0603n0:140_0": "Chabbassāniparakkamo;",
  "anya-e0603n0:141_0": "Oḷārikannapānehi;",
  "anya-e0603n0:142_0": "Dehaṁyāpesiṭhītiyā.",
  "anya-e0603n0:143_0": "29.",
  "anya-e0603n0:144_0": "Visākhapuṇṇamehema,",
  "anya-e0603n0:145_0": "Pātiṁpāyasapūritaṁ;",
  "anya-e0603n0:146_0": "Sujātāyābhataṁgayha,",
  "anya-e0603n0:147_0": "Nerañcaramupāgami.",
  "anya-e0603n0:148_0": "30.",
  "anya-e0603n0:149_0": "Nerañcarāyatīramhi,",
  "anya-e0603n0:150_0": "Sutvāpāyāsamuttamaṁ;",
  "anya-e0603n0:151_0": "Paṭiyatthavaramaggena,",
  "anya-e0603n0:152_0": "Bodhimaṇḍamupāgami.",
  "anya-e0603n0:153_0": "31.",
  "anya-e0603n0:154_0": "Jayāsanepallaṅkamhi,",
  "anya-e0603n0:155_0": "Ṭhitodhiṭṭhesidhītimā;",
  "anya-e0603n0:156_0": "Navīriyaṁjahissāmi,",
  "anya-e0603n0:157_0": "Apattoitibuddhataṁ.",
  "anya-e0603n0:158_0": "32.",
  "anya-e0603n0:159_0": "Devindodhamayaṁsaṅkhaṁ,",
  "anya-e0603n0:160_0": "Setacchattaṁpitāmaho;",
  "anya-e0603n0:161_0": "Dhārayaṁvādentovīṇaṁ,",
  "anya-e0603n0:162_0": "Pañcasīkhoṭhitotahiṁ.",
  "anya-e0603n0:163_0": "33.",
  "anya-e0603n0:164_0": "Thutigītānigāyantī,",
  "anya-e0603n0:165_0": "Aṭṭhāsuṁdevasundarī;",
  "anya-e0603n0:166_0": "Gahetvāhemamañjūsā,",
  "anya-e0603n0:167_0": "Surapupphehipūritā.",
  "anya-e0603n0:168_0": "34.",
  "anya-e0603n0:169_0": "Anatthamentesūriye,",
  "anya-e0603n0:170_0": "Mārasenaṁpalāpayī;",
  "anya-e0603n0:171_0": "Aruṇuggamanekāle,",
  "anya-e0603n0:172_0": "Pattosibuddhataṁvaraṁ.",
  "anya-e0603n0:173_0": "35.",
  "anya-e0603n0:174_0": "Paṭṭhānaṁsammasantassa,",
  "anya-e0603n0:175_0": "Nāthassadehatosubhā;",
  "anya-e0603n0:176_0": "Chabbaṇṇabhānicchadhanti,",
  "anya-e0603n0:177_0": "Vidhāvanti tahiṁ tahiṁ.",
  "anya-e0603n0:178_0": "36.",
  "anya-e0603n0:179_0": "Nīlānīlāpitāpītā,",
  "anya-e0603n0:180_0": "Rattārattācanicchare;",
  "anya-e0603n0:181_0": "Setāsetācamañciṭṭhā,",
  "anya-e0603n0:182_0": "Tamhātamhāpabhassarā.",
  "anya-e0603n0:183_0": "37.",
  "anya-e0603n0:184_0": "Buddhapattopibodheyya,",
  "anya-e0603n0:185_0": "Dhammagambhīrataṁjino;",
  "anya-e0603n0:186_0": "Nisāmetvānadesetuṁ,",
  "anya-e0603n0:187_0": "Nirussāhamupāgato.",
  "anya-e0603n0:188_0": "38.",
  "anya-e0603n0:189_0": "Sahampatitadābrahmā,",
  "anya-e0603n0:190_0": "Desanāyābhiyācati;",
  "anya-e0603n0:191_0": "Tenasopathamaṁseṭṭhaṁ,",
  "anya-e0603n0:192_0": "Dhammacakkamadesayi.",
  "anya-e0603n0:193_0": "39.",
  "anya-e0603n0:194_0": "Ekavassovasambuddho,",
  "anya-e0603n0:195_0": "Varephagguṇapuṇṇame;",
  "anya-e0603n0:196_0": "Sakyānaṁnagaraṁgañchī,",
  "anya-e0603n0:197_0": "Sāvakehipurakkhato.",
  "anya-e0603n0:198_0": "40.",
  "anya-e0603n0:199_0": "Samāgamamhiñātīnaṁ,",
  "anya-e0603n0:200_0": "Vassāpesisusītalaṁ;",
  "anya-e0603n0:201_0": "Mahāmeghañcadesesi,",
  "anya-e0603n0:202_0": "Vessantarassajātakaṁ.",
  "anya-e0603n0:203_0": "41.",
  "anya-e0603n0:204_0": "Gantvāsāvakayugena,",
  "anya-e0603n0:205_0": "Yasodharāyamandiraṁ;",
  "anya-e0603n0:206_0": "Taṁtosetvānadesesi,",
  "anya-e0603n0:207_0": "Candakinnarajātakaṁ.",
  "anya-e0603n0:208_0": "42.",
  "anya-e0603n0:209_0": "Vasitvāsattamevasse,",
  "anya-e0603n0:210_0": "Tāvatiṁse silāsane;",
  "anya-e0603n0:211_0": "Abhidhammena tosesi,",
  "anya-e0603n0:212_0": "Devabrahmagaṇaṁbahuṁ.",
  "anya-e0603n0:213_0": "43.",
  "anya-e0603n0:214_0": "Pattharaṁñāṇajālena,",
  "anya-e0603n0:215_0": "Lokaṁlokesisabbadā;",
  "anya-e0603n0:216_0": "Bodhetuṁbodhiyābhāya;",
  "anya-e0603n0:217_0": "Bodheyyajanapaṅkajaṁ.",
  "anya-e0603n0:218_0": "44.",
  "anya-e0603n0:219_0": "Bodhaneyyaṁjanaṁdisvā,",
  "anya-e0603n0:220_0": "Cakkavāḷaparaṁparā;",
  "anya-e0603n0:221_0": "Gantvāpivibodhesi,",
  "anya-e0603n0:222_0": "Buddhaseṭṭhosubodhimā.",
  "anya-e0603n0:223_0": "45.",
  "anya-e0603n0:224_0": "Bakādikecabrahmāno,",
  "anya-e0603n0:225_0": "Yakkhecāḷavakādike;",
  "anya-e0603n0:226_0": "Purindadādikedeve,",
  "anya-e0603n0:227_0": "Sūcilomādirakkhase.",
  "anya-e0603n0:228_0": "46.",
  "anya-e0603n0:229_0": "Jāṇusoṇādikeneke,",
  "anya-e0603n0:230_0": "Brāhmaṇecāvikakkhaḷe;",
  "anya-e0603n0:231_0": "Cūḷodarādikenāge,",
  "anya-e0603n0:232_0": "Dhanapālādikegaje.",
  "anya-e0603n0:233_0": "47.",
  "anya-e0603n0:234_0": "Buddhāmayaṁtimaññante,",
  "anya-e0603n0:235_0": "Titthiyenekaladdhike;",
  "anya-e0603n0:236_0": "Damesidamataṁseṭṭho,",
  "anya-e0603n0:237_0": "Paravādappamaddano.",
  "anya-e0603n0:238_0": "48.",
  "anya-e0603n0:239_0": "Anubyañjanasampanno,",
  "anya-e0603n0:240_0": "Esabāttiṁsalakkhaṇo;",
  "anya-e0603n0:241_0": "Byāmappabhāparikkhitto,",
  "anya-e0603n0:242_0": "Ketumālāsilaṅkhato.",
  "anya-e0603n0:243_0": "49.",
  "anya-e0603n0:244_0": "Caturāsītisahassa,",
  "anya-e0603n0:245_0": "Dhammakkhandhesadesayi;",
  "anya-e0603n0:246_0": "Pañcatāthīsavassāni,",
  "anya-e0603n0:247_0": "Tilokassahitāyave.",
  "anya-e0603n0:248_0": "50.",
  "anya-e0603n0:249_0": "Buddhakiccaṁkarontoso,",
  "anya-e0603n0:250_0": "Vahaṁlokahitaṁvaraṁ;",
  "anya-e0603n0:251_0": "Mocayaṁbandhanāsatte,",
  "anya-e0603n0:252_0": "Kālaṁkhepetisabbadā.",
  "anya-e0603n0:253_0": "51.",
  "anya-e0603n0:254_0": "Yahiṁ āsītikonātho,",
  "anya-e0603n0:255_0": "Nibbutovaramañjake;",
  "anya-e0603n0:256_0": "Mahāsālavaneramme,",
  "anya-e0603n0:257_0": "Mallānaṁnigametahiṁ.",
  "anya-e0603n0:258_0": "52.",
  "anya-e0603n0:259_0": "Aggikkhandhovalokasmiṁ,",
  "anya-e0603n0:260_0": "Jāletvānasasāvako;",
  "anya-e0603n0:261_0": "Nibbāyitthasaṅkhatānaṁ,",
  "anya-e0603n0:262_0": "Pakāsentoaniccataṁti.",
  "anya-e0603n0:263_0": "“Namotebhagavānātha,",
  "anya-e0603n0:264_0": "Namotejayataṁjaya;",
  "anya-e0603n0:265_0": "Namotesirisampanna,",
  "anya-e0603n0:266_0": "Namotelokanandana.”"
}