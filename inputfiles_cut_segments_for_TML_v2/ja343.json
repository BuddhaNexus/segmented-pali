{
  "ja343:0.1_0": "Jātaka",
  "ja343:0.2_0": "Catukkanipāta",
  "ja343:0.3_0": "Cūḷakuṇālavagga",
  "ja343:0.4_0": "3. Kuntinījātaka",
  "ja343:1.1_0": "“Avasimha tavāgāre,",
  "ja343:1.2_0": "niccaṁ sakkatapūjitā;",
  "ja343:1.3_0": "Tvameva dānimakari,",
  "ja343:1.4_0": "handa rāja vajāmahaṁ”.",
  "ja343:2.1_0": "“Yo ve kate paṭikate,",
  "ja343:2.2_0": "kibbise paṭikibbise;",
  "ja343:2.3_0": "Evaṁ taṁ sammatī veraṁ,",
  "ja343:2.4_0": "vasa kuntini māgamā”.",
  "ja343:3.1_0": "“Na katassa ca kattā ca,",
  "ja343:3.2_0": "metti sandhīyate puna;",
  "ja343:3.3_0": "Hadayaṁ nānujānāti,",
  "ja343:3.4_0": "gacchaññeva rathesabha”.",
  "ja343:4.1_0": "“Katassa ceva kattā ca,",
  "ja343:4.2_0": "metti sandhīyate puna;",
  "ja343:4.3_0": "Dhīrānaṁ no ca bālānaṁ,",
  "ja343:4.4_0": "vasa kuntini māgamā”ti.",
  "ja343:5.1_0": "Kuntinījātakaṁ tatiyaṁ."
}