{
  "thig1.3:0.1_0": "Therīgāthā",
  "thig1.3:0.2_0": "Ekakanipāta",
  "thig1.3:0.3_0": "3. Puṇṇātherīgāthā",
  "thig1.3:1.1_0": "“Puṇṇe pūrassu dhammehi,",
  "thig1.3:1.2_0": "Cando pannaraseriva;",
  "thig1.3:1.3_0": "Paripuṇṇāya paññāya,",
  "thig1.3:1.4_0": "Tamokhandhaṁ padālayā”ti.",
  "thig1.3:2.1_0": "Itthaṁ sudaṁ puṇṇā therī gāthaṁ abhāsitthāti."
}