{
  "ja412:0.1_0": "Jātaka",
  "ja412:0.2_0": "Sattakanipāta",
  "ja412:0.3_0": "Gandhāravagga",
  "ja412:0.4_0": "7. Koṭasimbalijātaka",
  "ja412:1.1_0": "“Ahaṁ dasasataṁbyāmaṁ,",
  "ja412:1.2_0": "uragamādāya āgato;",
  "ja412:1.3_0": "Tañca mañca mahākāyaṁ,",
  "ja412:1.4_0": "dhārayaṁ nappavedhasi.",
  "ja412:2.1_0": "Athimaṁ khuddakaṁ pakkhiṁ,",
  "ja412:2.2_0": "Appamaṁsataraṁ mayā;",
  "ja412:2.3_0": "Dhārayaṁ byathasi bhītā,",
  "ja412:2.4_0": "Kamatthaṁ koṭasimbali”.",
  "ja412:3.1_0": "“Maṁsabhakkho tuvaṁ rāja,",
  "ja412:3.2_0": "phalabhakkho ayaṁ dijo;",
  "ja412:3.3_0": "Ayaṁ nigrodhabījāni,",
  "ja412:3.4_0": "pilakkhudumbarāni ca;",
  "ja412:3.5_0": "Assatthāni ca bhakkhitvā,",
  "ja412:3.6_0": "khandhe me ohadissati.",
  "ja412:4.1_0": "Te rukkhā saṁvirūhanti,",
  "ja412:4.2_0": "mama passe nivātajā;",
  "ja412:4.3_0": "Te maṁ pariyonandhissanti,",
  "ja412:4.4_0": "arukkhaṁ maṁ karissare.",
  "ja412:5.1_0": "Santi aññepi rukkhā se,",
  "ja412:5.2_0": "mūlino khandhino dumā;",
  "ja412:5.3_0": "Iminā sakuṇajātena,",
  "ja412:5.4_0": "bījamāharitā hatā.",
  "ja412:6.1_0": "Ajjhārūhābhivaḍḍhanti,",
  "ja412:6.2_0": "brahantampi vanappatiṁ;",
  "ja412:6.3_0": "Tasmā rāja pavedhāmi,",
  "ja412:6.4_0": "sampassaṁnāgataṁ bhayaṁ”.",
  "ja412:7.1_0": "“Saṅkeyya saṅkitabbāni,",
  "ja412:7.2_0": "rakkheyyānāgataṁ bhayaṁ;",
  "ja412:7.3_0": "Anāgatabhayā dhīro,",
  "ja412:7.4_0": "ubho loke avekkhatī”ti.",
  "ja412:8.1_0": "Koṭasimbalijātakaṁ sattamaṁ."
}