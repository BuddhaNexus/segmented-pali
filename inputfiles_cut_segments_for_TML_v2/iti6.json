{
  "iti6:0.1_0": "Itivuttaka 6",
  "iti6:0.2_0": "Ekakanipāta",
  "iti6:0.3_0": "Paṭhamavagga",
  "iti6:0.4_0": "Mānasutta",
  "iti6:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti6:2.1_0": "“Ekadhammaṁ, bhikkhave, pajahatha;",
  "iti6:2.2_0": "ahaṁ vo pāṭibhogo anāgāmitāya.",
  "iti6:2.3_0": "Katamaṁ ekadhammaṁ?",
  "iti6:2.4_0": "Mānaṁ, bhikkhave, ekadhammaṁ pajahatha;",
  "iti6:2.5_0": "ahaṁ vo pāṭibhogo anāgāmitāyā”ti.",
  "iti6:2.6_0": "Etamatthaṁ bhagavā avoca.",
  "iti6:2.7_0": "Tatthetaṁ iti vuccati:",
  "iti6:3.1_0": "“Yena mānena mattāse,",
  "iti6:3.2_0": "sattā gacchanti duggatiṁ;",
  "iti6:3.3_0": "Taṁ mānaṁ sammadaññāya,",
  "iti6:3.4_0": "pajahanti vipassino;",
  "iti6:3.5_0": "Pahāya na punāyanti,",
  "iti6:3.6_0": "imaṁ lokaṁ kudācanan”ti.",
  "iti6:4.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti6:4.2_0": "Chaṭṭhaṁ."
}