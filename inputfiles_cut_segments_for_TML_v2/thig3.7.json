{
  "thig3.7:0.1_0": "Therīgāthā",
  "thig3.7:0.2_0": "Tikanipāta",
  "thig3.7:0.3_0": "7. Selātherīgāthā",
  "thig3.7:1.1_0": "“Natthi nissaraṇaṁ loke,",
  "thig3.7:1.2_0": "kiṁ vivekena kāhasi;",
  "thig3.7:1.3_0": "Bhuñjāhi kāmaratiyo,",
  "thig3.7:1.4_0": "māhu pacchānutāpinī”.",
  "thig3.7:2.1_0": "“Sattisūlūpamā kāmā,",
  "thig3.7:2.2_0": "khandhāsaṁ adhikuṭṭanā;",
  "thig3.7:2.3_0": "Yaṁ tvaṁ ‘kāmaratiṁ’ brūsi,",
  "thig3.7:2.4_0": "‘aratī’ dāni sā mama.",
  "thig3.7:3.1_0": "Sabbattha vihatā nandī,",
  "thig3.7:3.2_0": "Tamokhandho padālito;",
  "thig3.7:3.3_0": "Evaṁ jānāhi pāpima,",
  "thig3.7:3.4_0": "Nihato tvamasi antakā”ti.",
  "thig3.7:4.1_0": "… Selā therī …."
}