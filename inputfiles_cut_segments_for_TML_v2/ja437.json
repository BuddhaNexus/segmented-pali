{
  "ja437:0.1_0": "Jātaka",
  "ja437:0.2_0": "Navakanipāta",
  "ja437:0.3_0": "Gijjhavagga",
  "ja437:0.4_0": "11. Pūtimaṁsajātaka",
  "ja437:1.1_0": "“Na kho me ruccati āḷi,",
  "ja437:1.2_0": "pūtimaṁsassa pekkhanā;",
  "ja437:1.3_0": "Etādisā sakhārasmā,",
  "ja437:1.4_0": "ārakā parivajjaye”.",
  "ja437:2.1_0": "“Ummattikā ayaṁ veṇī,",
  "ja437:2.2_0": "vaṇṇeti patino sakhiṁ;",
  "ja437:2.3_0": "Pajjhāyi paṭigacchantiṁ,",
  "ja437:2.4_0": "āgataṁ meṇḍamātaraṁ”.",
  "ja437:3.1_0": "“Tvaṁ khosi samma ummatto,",
  "ja437:3.2_0": "dummedho avicakkhaṇo;",
  "ja437:3.3_0": "Yo tvaṁ matālayaṁ katvā,",
  "ja437:3.4_0": "akālena vipekkhasi”.",
  "ja437:4.1_0": "“Na akāle vipekkheyya,",
  "ja437:4.2_0": "kāle pekkheyya paṇḍito;",
  "ja437:4.3_0": "Pūtimaṁsova pajjhāyi,",
  "ja437:4.4_0": "yo akāle vipekkhati”.",
  "ja437:5.1_0": "“Piyaṁ kho āḷi me hotu,",
  "ja437:5.2_0": "puṇṇapattaṁ dadāhi me;",
  "ja437:5.3_0": "Pati sañjīvito mayhaṁ,",
  "ja437:5.4_0": "eyyāsi piyapucchikā”.",
  "ja437:6.1_0": "“Piyaṁ kho āḷi te hotu,",
  "ja437:6.2_0": "puṇṇapattaṁ dadāmi te;",
  "ja437:6.3_0": "Mahatā parivārena,",
  "ja437:6.4_0": "essaṁ kayirāhi bhojanaṁ”.",
  "ja437:7.1_0": "“Kīdiso tuyhaṁ parivāro,",
  "ja437:7.2_0": "yesaṁ kāhāmi bhojanaṁ;",
  "ja437:7.3_0": "Kiṁnāmakā ca te sabbe,",
  "ja437:7.4_0": "taṁ me akkhāhi pucchitā”.",
  "ja437:8.1_0": "“Māliyo caturakkho ca,",
  "ja437:8.2_0": "piṅgiyo atha jambuko;",
  "ja437:8.3_0": "Ediso mayhaṁ parivāro,",
  "ja437:8.4_0": "tesaṁ kayirāhi bhojanaṁ”.",
  "ja437:9.1_0": "“Nikkhantāya agārasmā,",
  "ja437:9.2_0": "bhaṇḍakampi vinassati;",
  "ja437:9.3_0": "Ārogyaṁ āḷino vajjaṁ,",
  "ja437:9.4_0": "idheva vasa māgamā”ti.",
  "ja437:10.1_0": "Pūtimaṁsajātakaṁ ekādasamaṁ."
}