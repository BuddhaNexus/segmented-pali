{
  "ja371:0.1_0": "Jātaka",
  "ja371:0.2_0": "Pañcakanipāta",
  "ja371:0.3_0": "Aḍḍhavagga",
  "ja371:0.4_0": "1. Dīghītikosalajātaka",
  "ja371:1.1_0": "“Evaṁbhūtassa te rāja,",
  "ja371:1.2_0": "āgatassa vase mama;",
  "ja371:1.3_0": "Atthi nu koci pariyāyo,",
  "ja371:1.4_0": "yo taṁ dukkhā pamocaye”.",
  "ja371:2.1_0": "“Evaṁbhūtassa me tāta,",
  "ja371:2.2_0": "āgatassa vase tava;",
  "ja371:2.3_0": "Natthi no koci pariyāyo,",
  "ja371:2.4_0": "yo maṁ dukkhā pamocaye”.",
  "ja371:3.1_0": "“Nāññaṁ sucaritaṁ rāja,",
  "ja371:3.2_0": "nāññaṁ rāja subhāsitaṁ;",
  "ja371:3.3_0": "Tāyate maraṇakāle,",
  "ja371:3.4_0": "evamevitaraṁ dhanaṁ.",
  "ja371:4.1_0": "Akkocchi maṁ avadhi maṁ,",
  "ja371:4.2_0": "ajini maṁ ahāsi me;",
  "ja371:4.3_0": "Ye ca taṁ upanayhanti,",
  "ja371:4.4_0": "veraṁ tesaṁ na sammati.",
  "ja371:5.1_0": "Akkocchi maṁ avadhi maṁ,",
  "ja371:5.2_0": "ajini maṁ ahāsi me;",
  "ja371:5.3_0": "Ye ca taṁ nupanayhanti,",
  "ja371:5.4_0": "veraṁ tesūpasammati.",
  "ja371:6.1_0": "Na hi verena verāni,",
  "ja371:6.2_0": "sammantīdha kudācanaṁ;",
  "ja371:6.3_0": "Averena ca sammanti,",
  "ja371:6.4_0": "esa dhammo sanantano”ti.",
  "ja371:7.1_0": "Dīghītikosalajātakaṁ paṭhamaṁ."
}