{
  "thag1.72:0.1_0": "Theragāthā",
  "thag1.72:0.2_0": "Ekakanipāta",
  "thag1.72:0.3_0": "Aṭṭhamavagga",
  "thag1.72:0.4_0": "1.72. Ātumattheragāthā",
  "thag1.72:1.1_0": "“Yathā kaḷīro susu vaḍḍhitaggo,",
  "thag1.72:1.2_0": "Dunnikkhamo hoti pasākhajāto;",
  "thag1.72:1.3_0": "Evaṁ ahaṁ bhariyāyānitāya,",
  "thag1.72:1.4_0": "Anumaññaṁ maṁ pabbajitomhi dānī”ti.",
  "thag1.72:1.5_0": "… Ātumo thero …."
}