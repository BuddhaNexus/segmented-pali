{
  "pv44:0.1_0": "Petavatthu",
  "pv44:0.2_0": "Mahāvagga",
  "pv44:0.3_0": "9. Gūthakhādakapetivatthu",
  "pv44:1.1_0": "“Gūthakūpato uggantvā,",
  "pv44:1.2_0": "kā nu dīnā patiṭṭhasi;",
  "pv44:1.3_0": "Nissaṁsayaṁ pāpakammantā,",
  "pv44:1.4_0": "kiṁ nu saddahase tuvan”ti.",
  "pv44:2.1_0": "“Ahaṁ bhadante petīmhi,",
  "pv44:2.2_0": "duggatā yamalokikā;",
  "pv44:2.3_0": "Pāpakammaṁ karitvāna,",
  "pv44:2.4_0": "petalokaṁ ito gatā”ti.",
  "pv44:3.1_0": "“Kiṁ nu kāyena vācāya,",
  "pv44:3.2_0": "manasā dukkaṭaṁ kataṁ;",
  "pv44:3.3_0": "Kissa kammavipākena,",
  "pv44:3.4_0": "idaṁ dukkhaṁ nigacchasī”ti.",
  "pv44:4.1_0": "“Ahu āvāsiko mayhaṁ,",
  "pv44:4.2_0": "issukī kulamaccharī;",
  "pv44:4.3_0": "Ajjhāsito mayhaṁ ghare,",
  "pv44:4.4_0": "kadariyo paribhāsako.",
  "pv44:5.1_0": "Tassāhaṁ vacanaṁ sutvā,",
  "pv44:5.2_0": "bhikkhavo paribhāsisaṁ;",
  "pv44:5.3_0": "Tassa kammavipākena,",
  "pv44:5.4_0": "petalokaṁ ito gatā”ti.",
  "pv44:6.1_0": "“Amitto mittavaṇṇena,",
  "pv44:6.2_0": "yo te āsi kulūpako;",
  "pv44:6.3_0": "Kāyassa bhedā duppañño,",
  "pv44:6.4_0": "kiṁ nu pecca gatiṁ gato”ti.",
  "pv44:7.1_0": "“Tassevāhaṁ pāpakammassa,",
  "pv44:7.2_0": "sīse tiṭṭhāmi matthake;",
  "pv44:7.3_0": "So ca paravisayaṁ patto,",
  "pv44:7.4_0": "mameva paricārako.",
  "pv44:8.1_0": "Yaṁ bhadante hadantaññe,",
  "pv44:8.2_0": "etaṁ me hoti bhojanaṁ;",
  "pv44:8.3_0": "Ahañca kho yaṁ hadāmi,",
  "pv44:8.4_0": "etaṁ so upajīvatī”ti.",
  "pv44:9.1_0": "Gūthakhādakapetivatthu navamaṁ."
}