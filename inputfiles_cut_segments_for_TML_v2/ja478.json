{
  "ja478:0.1_0": "Jātaka",
  "ja478:0.2_0": "Terasakanipāta",
  "ja478:0.3_0": "Ambavagga",
  "ja478:0.4_0": "5. Dūtajātaka",
  "ja478:1.1_0": "“Dūte te brahme pāhesiṁ,",
  "ja478:1.2_0": "Gaṅgātīrasmi jhāyato;",
  "ja478:1.3_0": "Tesaṁ puṭṭho na byākāsi,",
  "ja478:1.4_0": "Dukkhaṁ guyhamataṁ nu te”.",
  "ja478:2.1_0": "“Sace te dukkhamuppajje,",
  "ja478:2.2_0": "kāsīnaṁ raṭṭhavaḍḍhana;",
  "ja478:2.3_0": "Mā kho naṁ tassa akkhāhi,",
  "ja478:2.4_0": "yo taṁ dukkhā na mocaye.",
  "ja478:3.1_0": "Yo tassa dukkhajātassa,",
  "ja478:3.2_0": "ekaṅgamapi bhāgaso;",
  "ja478:3.3_0": "Vippamoceyya dhammena,",
  "ja478:3.4_0": "kāmaṁ tassa pavedaya.",
  "ja478:4.1_0": "Suvijānaṁ siṅgālānaṁ,",
  "ja478:4.2_0": "sakuṇānañca vassitaṁ;",
  "ja478:4.3_0": "Manussavassitaṁ rāja,",
  "ja478:4.4_0": "dubbijānataraṁ tato.",
  "ja478:5.1_0": "Api ce maññatī poso,",
  "ja478:5.2_0": "ñāti mitto sakhāti vā;",
  "ja478:5.3_0": "Yo pubbe sumano hutvā,",
  "ja478:5.4_0": "pacchā sampajjate diso.",
  "ja478:6.1_0": "Yo attano dukkhamanānupuṭṭho,",
  "ja478:6.2_0": "Pavedaye jantu akālarūpe;",
  "ja478:6.3_0": "Ānandino tassa bhavantimittā,",
  "ja478:6.4_0": "Hitesino tassa dukhī bhavanti.",
  "ja478:7.1_0": "Kālañca ñatvāna tathāvidhassa,",
  "ja478:7.2_0": "Medhāvīnaṁ ekamanaṁ viditvā;",
  "ja478:7.3_0": "Akkheyya tibbāni parassa dhīro,",
  "ja478:7.4_0": "Saṇhaṁ giraṁ atthavatiṁ pamuñce.",
  "ja478:8.1_0": "Sace ca jaññā avisayhamattano,",
  "ja478:8.2_0": "Na te hi mayhaṁ sukhāgamāya;",
  "ja478:8.3_0": "Ekova tibbāni saheyya dhīro,",
  "ja478:8.4_0": "Saccaṁ hirottappamapekkhamāno.",
  "ja478:9.1_0": "Ahaṁ raṭṭhe vicaranto,",
  "ja478:9.2_0": "nigame rājadhāniyo;",
  "ja478:9.3_0": "Bhikkhamāno mahārāja,",
  "ja478:9.4_0": "ācariyassa dhanatthiko.",
  "ja478:10.1_0": "Gahapatī rājapurise,",
  "ja478:10.2_0": "mahāsāle ca brāhmaṇe;",
  "ja478:10.3_0": "Alatthaṁ satta nikkhāni,",
  "ja478:10.4_0": "suvaṇṇassa janādhipa;",
  "ja478:10.5_0": "Te me naṭṭhā mahārāja,",
  "ja478:10.6_0": "tasmā socāmahaṁ bhusaṁ.",
  "ja478:11.1_0": "Purisā te mahārāja,",
  "ja478:11.2_0": "manasānuvicintitā;",
  "ja478:11.3_0": "Nālaṁ dukkhā pamocetuṁ,",
  "ja478:11.4_0": "tasmā tesaṁ na byāhariṁ.",
  "ja478:12.1_0": "Tvañca kho me mahārāja,",
  "ja478:12.2_0": "manasānuvicintito;",
  "ja478:12.3_0": "Alaṁ dukkhā pamocetuṁ,",
  "ja478:12.4_0": "tasmā tuyhaṁ pavedayiṁ”.",
  "ja478:13.1_0": "“Tassādāsi pasannatto,",
  "ja478:13.2_0": "kāsīnaṁ raṭṭhavaḍḍhano;",
  "ja478:13.3_0": "Jātarūpamaye nikkhe,",
  "ja478:13.4_0": "suvaṇṇassa catuddasā”ti.",
  "ja478:14.1_0": "Dūtajātakaṁ pañcamaṁ."
}