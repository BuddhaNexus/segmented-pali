{
  "iti58:0.1_0": "Itivuttaka 58",
  "iti58:0.2_0": "Tikanipāta",
  "iti58:0.3_0": "Paṭhamavagga",
  "iti58:0.4_0": "Taṇhāsutta",
  "iti58:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti58:2.1_0": "“Tisso imā, bhikkhave, taṇhā.",
  "iti58:2.2_0": "Katamā tisso?",
  "iti58:2.3_0": "Kāmataṇhā, bhavataṇhā, vibhavataṇhā—",
  "iti58:2.4_0": "imā kho, bhikkhave, tisso taṇhā”ti.",
  "iti58:2.5_0": "Etamatthaṁ bhagavā avoca.",
  "iti58:2.6_0": "Tatthetaṁ iti vuccati:",
  "iti58:3.1_0": "“Taṇhāyogena saṁyuttā,",
  "iti58:3.2_0": "rattacittā bhavābhave;",
  "iti58:3.3_0": "Te yogayuttā mārassa,",
  "iti58:3.4_0": "ayogakkhemino janā;",
  "iti58:3.5_0": "Sattā gacchanti saṁsāraṁ,",
  "iti58:3.6_0": "jātīmaraṇagāmino.",
  "iti58:4.1_0": "Ye ca taṇhaṁ pahantvāna,",
  "iti58:4.2_0": "vītataṇhā bhavābhave;",
  "iti58:4.3_0": "Te ve pāraṅgatā loke,",
  "iti58:4.4_0": "ye pattā āsavakkhayan”ti.",
  "iti58:5.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti58:5.2_0": "Navamaṁ."
}