{
  "iti72:0.1_0": "Itivuttaka 72",
  "iti72:0.2_0": "Tikanipāta",
  "iti72:0.3_0": "Tatiyavagga",
  "iti72:0.4_0": "Nissaraṇiyasutta",
  "iti72:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti72:2.1_0": "“Tisso imā, bhikkhave, nissaraṇiyā dhātuyo.",
  "iti72:2.2_0": "Katamā tisso?",
  "iti72:2.3_0": "Kāmānametaṁ nissaraṇaṁ yadidaṁ nekkhammaṁ, rūpānametaṁ nissaraṇaṁ yadidaṁ āruppaṁ, yaṁ kho pana kiñci bhūtaṁ saṅkhataṁ paṭiccasamuppannaṁ nirodho tassa nissaraṇaṁ—",
  "iti72:2.4_0": "imā kho, bhikkhave, tisso nissaraṇiyā dhātuyo”ti.",
  "iti72:2.5_0": "Etamatthaṁ bhagavā avoca.",
  "iti72:2.6_0": "Tatthetaṁ iti vuccati:",
  "iti72:3.1_0": "“Kāmanissaraṇaṁ ñatvā,",
  "iti72:3.2_0": "rūpānañca atikkamaṁ;",
  "iti72:3.3_0": "Sabbasaṅkhārasamathaṁ,",
  "iti72:3.4_0": "phusaṁ ātāpi sabbadā.",
  "iti72:4.1_0": "Sa ve sammaddaso bhikkhu,",
  "iti72:4.2_0": "yato tattha vimuccati;",
  "iti72:4.3_0": "Abhiññāvosito santo,",
  "iti72:4.4_0": "sa ve yogātigo munī”ti.",
  "iti72:5.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti72:5.2_0": "Tatiyaṁ."
}