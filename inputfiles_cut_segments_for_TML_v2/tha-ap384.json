{
  "tha-ap384:0.1_0": "Therāpadāna",
  "tha-ap384:0.2_0": "Avaṭaphalavagga",
  "tha-ap384:0.3_0": "2. Labujadāyakattheraapadāna",
  "tha-ap384:1.1_0": "“Nagare bandhumatiyā,",
  "tha-ap384:1.2_0": "āsiṁ ārāmiko tadā;",
  "tha-ap384:1.3_0": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap384:1.4_0": "gacchantaṁ anilañjase.",
  "tha-ap384:2.1_0": "Labujassa phalaṁ gayha,",
  "tha-ap384:2.2_0": "buddhaseṭṭhassadāsahaṁ;",
  "tha-ap384:2.3_0": "Ākāse ṭhitako santo,",
  "tha-ap384:2.4_0": "paṭigaṇhi mahāyaso.",
  "tha-ap384:3.1_0": "Vittisañjananaṁ mayhaṁ,",
  "tha-ap384:3.2_0": "diṭṭhadhammasukhāvahaṁ;",
  "tha-ap384:3.3_0": "Phalaṁ buddhassa datvāna,",
  "tha-ap384:3.4_0": "vippasannena cetasā.",
  "tha-ap384:4.1_0": "Adhigacchiṁ tadā pītiṁ,",
  "tha-ap384:4.2_0": "vipulañca sukhuttamaṁ;",
  "tha-ap384:4.3_0": "Uppajjateva ratanaṁ,",
  "tha-ap384:4.4_0": "nibbattassa tahiṁ tahiṁ.",
  "tha-ap384:5.1_0": "Ekanavutito kappe,",
  "tha-ap384:5.2_0": "yaṁ phalamadadiṁ tadā;",
  "tha-ap384:5.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap384:5.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap384:6.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap384:6.2_0": "…pe…",
  "tha-ap384:6.3_0": "viharāmi anāsavo.",
  "tha-ap384:7.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap384:7.2_0": "…pe…",
  "tha-ap384:7.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap384:8.1_0": "Paṭisambhidā catasso,",
  "tha-ap384:8.2_0": "…pe…",
  "tha-ap384:8.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap384:9.1_0": "Itthaṁ sudaṁ āyasmā labujadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap384:10.1_0": "Labujadāyakattherassāpadānaṁ dutiyaṁ."
}