{
  "ja18:0.1_0": "Jātaka",
  "ja18:0.2_0": "Ekakanipāta",
  "ja18:0.3_0": "Sīlavagga",
  "ja18:0.4_0": "8. Matakabhattajātaka",
  "ja18:1.1_0": "“Evañce sattā jāneyyuṁ,",
  "ja18:1.2_0": "dukkhāyaṁ jātisambhavo;",
  "ja18:1.3_0": "Na pāṇo pāṇinaṁ haññe,",
  "ja18:1.4_0": "pāṇaghātī hi socatī”ti.",
  "ja18:2.1_0": "Matakabhattajātakaṁ aṭṭhamaṁ."
}