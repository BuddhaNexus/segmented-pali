{
  "ja36:0.1_0": "Jātaka",
  "ja36:0.2_0": "Ekakanipāta",
  "ja36:0.3_0": "Kulāvakavagga",
  "ja36:0.4_0": "6. Sakuṇajātaka",
  "ja36:1.1_0": "“Yaṁ nissitā jagatiruhaṁ vihaṅgamā,",
  "ja36:1.2_0": "Svāyaṁ aggiṁ pamuñcati;",
  "ja36:1.3_0": "Disā bhajatha vakkaṅgā,",
  "ja36:1.4_0": "Jātaṁ saraṇato bhayan”ti.",
  "ja36:2.1_0": "Sakuṇajātakaṁ chaṭṭhaṁ."
}