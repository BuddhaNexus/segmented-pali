{
  "tha-ap509:0.1_0": "Therāpadāna",
  "tha-ap509:0.2_0": "Kaṇikāravagga",
  "tha-ap509:0.3_0": "7 Ajeliphaladāyakattheraapadāna",
  "tha-ap509:1.1_0": "“Ajjuno nāma sambuddho,",
  "tha-ap509:1.2_0": "himavante vasī tadā;",
  "tha-ap509:1.3_0": "Caraṇena ca sampanno,",
  "tha-ap509:1.4_0": "samādhikusalo muni.",
  "tha-ap509:2.1_0": "Kumbhamattaṁ gahetvāna,",
  "tha-ap509:2.2_0": "ajeliṁ jīvajīvakaṁ;",
  "tha-ap509:2.3_0": "Chattapaṇṇaṁ gahetvāna,",
  "tha-ap509:2.4_0": "adāsiṁ satthuno ahaṁ.",
  "tha-ap509:3.1_0": "Catunnavutito kappe,",
  "tha-ap509:3.2_0": "yaṁ phalaṁ adadiṁ tadā;",
  "tha-ap509:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap509:3.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap509:4.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap509:4.2_0": "…pe…",
  "tha-ap509:4.3_0": "viharāmi anāsavo.",
  "tha-ap509:5.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap509:5.2_0": "…pe…",
  "tha-ap509:5.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap509:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap509:6.2_0": "…pe…",
  "tha-ap509:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap509:7.1_0": "Itthaṁ sudaṁ āyasmā ajeliphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap509:8.1_0": "Ajeliphaladāyakattherassāpadānaṁ sattamaṁ."
}