{
  "thig1.1:0.1_0": "Therīgāthā",
  "thig1.1:0.2_0": "Ekakanipāta",
  "thig1.1:0.3_0": "1. Aññatarātherīgāthā",
  "thig1.1:1.0_0": "Namo tassa Bhagavato Arahato Sammāsambuddhassa.",
  "thig1.1:1.1_0": "“Sukhaṁ supāhi therike,",
  "thig1.1:1.2_0": "Katvā coḷena pārutā;",
  "thig1.1:1.3_0": "Upasanto hi te rāgo,",
  "thig1.1:1.4_0": "Sukkhaḍākaṁ va kumbhiyan”ti.",
  "thig1.1:2.1_0": "Itthaṁ sudaṁ aññatarā therī apaññātā bhikkhunī gāthaṁ abhāsitthāti."
}