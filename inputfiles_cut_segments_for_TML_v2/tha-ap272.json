{
  "tha-ap272:0.1_0": "Therāpadāna",
  "tha-ap272:0.2_0": "Padumukkhipavagga",
  "tha-ap272:0.3_0": "10. Taraṇiyattheraapadāna",
  "tha-ap272:1.1_0": "“Suvaṇṇavaṇṇo sambuddho,",
  "tha-ap272:1.2_0": "vipassī dakkhiṇāraho;",
  "tha-ap272:1.3_0": "Nadītīre ṭhito satthā,",
  "tha-ap272:1.4_0": "bhikkhusaṅghapurakkhato.",
  "tha-ap272:2.1_0": "Nāvā na vijjate tattha,",
  "tha-ap272:2.2_0": "santāraṇī mahaṇṇave;",
  "tha-ap272:2.3_0": "Nadiyā abhinikkhamma,",
  "tha-ap272:2.4_0": "tāresiṁ lokanāyakaṁ.",
  "tha-ap272:3.1_0": "Ekanavutito kappe,",
  "tha-ap272:3.2_0": "yaṁ tāresiṁ naruttamaṁ;",
  "tha-ap272:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap272:3.4_0": "taraṇāya idaṁ phalaṁ.",
  "tha-ap272:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap272:4.2_0": "…pe…",
  "tha-ap272:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap272:5.1_0": "Itthaṁ sudaṁ āyasmā taraṇiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap272:6.1_0": "Taraṇiyattherassāpadānaṁ dasamaṁ.",
  "tha-ap272:7.1_0": "Padumukkhipavaggo sattavīsatimo.",
  "tha-ap272:8.1_0": "Tassuddānaṁ",
  "tha-ap272:9.1_0": "Ukkhipī telacandī ca,",
  "tha-ap272:9.2_0": "dīpado ca biḷālido;",
  "tha-ap272:9.3_0": "Maccho javo saḷalado,",
  "tha-ap272:9.4_0": "rakkhaso taraṇo dasa;",
  "tha-ap272:9.5_0": "Gāthāyo cettha saṅkhātā,",
  "tha-ap272:9.6_0": "tālīsaṁ cekameva cāti."
}