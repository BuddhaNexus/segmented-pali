{
  "pv9:0.1_0": "Petavatthu",
  "pv9:0.2_0": "Uragavagga",
  "pv9:0.3_0": "9. Mahāpesakārapetivatthu",
  "pv9:1.1_0": "“Gūthañca muttaṁ ruhirañca pubbaṁ,",
  "pv9:1.2_0": "Paribhuñjati kissa ayaṁ vipāko;",
  "pv9:1.3_0": "Ayaṁ nu kiṁ kammamakāsi nārī,",
  "pv9:1.4_0": "Yā sabbadā lohitapubbabhakkhā.",
  "pv9:2.1_0": "Navāni vatthāni subhāni ceva,",
  "pv9:2.2_0": "Mudūni suddhāni ca lomasāni;",
  "pv9:2.3_0": "Dinnānimissā kitakā bhavanti,",
  "pv9:2.4_0": "Ayaṁ nu kiṁ kammamakāsi nārī”ti.",
  "pv9:3.1_0": "“Bhariyā mamesā ahū bhadante,",
  "pv9:3.2_0": "Adāyikā maccharinī kadariyā;",
  "pv9:3.3_0": "Sā maṁ dadantaṁ samaṇabrāhmaṇānaṁ,",
  "pv9:3.4_0": "Akkosati ca paribhāsati ca.",
  "pv9:4.1_0": "‘Gūthañca muttaṁ ruhirañca pubbaṁ,",
  "pv9:4.2_0": "Paribhuñja tvaṁ asuciṁ sabbakālaṁ;",
  "pv9:4.3_0": "Etaṁ te paralokasmiṁ hotu,",
  "pv9:4.4_0": "Vatthā ca te kitakasamā bhavantu’;",
  "pv9:4.5_0": "Etādisaṁ duccaritaṁ caritvā,",
  "pv9:4.6_0": "Idhāgatā cirarattāya khādatī”ti.",
  "pv9:5.1_0": "Mahāpesakārapetivatthu navamaṁ."
}