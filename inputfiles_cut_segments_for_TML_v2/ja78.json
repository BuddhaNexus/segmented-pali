{
  "ja78:0.1_0": "Jātaka",
  "ja78:0.2_0": "Ekakanipāta",
  "ja78:0.3_0": "Varuṇavagga",
  "ja78:0.4_0": "8. Illisajātaka",
  "ja78:1.1_0": "“Ubho khañjā ubho kuṇī,",
  "ja78:1.2_0": "ubho visamacakkhukā;",
  "ja78:1.3_0": "Ubhinnaṁ piḷakā jātā,",
  "ja78:1.4_0": "nāhaṁ passāmi illisan”ti.",
  "ja78:2.1_0": "Illisajātakaṁ aṭṭhamaṁ."
}