{
  "pli-tv-bu-vb-sk31:0.1_0": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk31:0.2_0": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk31:0.3_0": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk31:0.4_0": "Sakkaccavagga",
  "pli-tv-bu-vb-sk31:0.5_0": "31. Sakkaccasikkhāpada",
  "pli-tv-bu-vb-sk31:1.1_0": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk31:1.2_0": "Tena kho pana samayena chabbaggiyā bhikkhū asakkaccaṁ piṇḍapātaṁ bhuñjanti abhuñjitukāmā viya …pe….",
  "pli-tv-bu-vb-sk31:1.3.1_0": "“Sakkaccaṁ piṇḍapātaṁ bhuñjissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk31:1.4_0": "Sakkaccaṁ piṇḍapāto bhuñjitabbo.",
  "pli-tv-bu-vb-sk31:1.5_0": "Yo anādariyaṁ paṭicca asakkaccaṁ piṇḍapātaṁ bhuñjati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk31:1.6.1_0": "Anāpatti—",
  "pli-tv-bu-vb-sk31:1.7_0": "asañcicca,",
  "pli-tv-bu-vb-sk31:1.8_0": "assatiyā,",
  "pli-tv-bu-vb-sk31:1.9_0": "ajānantassa,",
  "pli-tv-bu-vb-sk31:1.10_0": "gilānassa,",
  "pli-tv-bu-vb-sk31:1.11_0": "āpadāsu,",
  "pli-tv-bu-vb-sk31:1.12_0": "ummattakassa,",
  "pli-tv-bu-vb-sk31:1.13_0": "ādikammikassāti.",
  "pli-tv-bu-vb-sk31:1.14_0": "Paṭhamasikkhāpadaṁ niṭṭhitaṁ."
}