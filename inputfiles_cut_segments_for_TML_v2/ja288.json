{
  "ja288:0.1_0": "Jātaka",
  "ja288:0.2_0": "Tikanipāta",
  "ja288:0.3_0": "Abbhantaravagga",
  "ja288:0.4_0": "8. Macchuddānajātaka",
  "ja288:1.1_0": "“Agghanti macchā adhikaṁ sahassaṁ,",
  "ja288:1.2_0": "Na so atthi yo imaṁ saddaheyya;",
  "ja288:1.3_0": "Mayhañca assu idha satta māsā,",
  "ja288:1.4_0": "Ahampi taṁ macchuddānaṁ kiṇeyyaṁ”.",
  "ja288:2.1_0": "“Macchānaṁ bhojanaṁ datvā,",
  "ja288:2.2_0": "mama dakkhiṇamādisi;",
  "ja288:2.3_0": "Taṁ dakkhiṇaṁ sarantiyā,",
  "ja288:2.4_0": "kataṁ apacitiṁ tayā”.",
  "ja288:3.1_0": "“Paduṭṭhacittassa na phāti hoti,",
  "ja288:3.2_0": "Na cāpi taṁ devatā pūjayanti;",
  "ja288:3.3_0": "Yo bhātaraṁ pettikaṁ sāpateyyaṁ,",
  "ja288:3.4_0": "Avañcayī dukkaṭakammakārī”ti.",
  "ja288:4.1_0": "Macchuddānajātakaṁ aṭṭhamaṁ."
}