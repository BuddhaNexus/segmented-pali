{
  "tha-ap379:0.1_0": "Therāpadāna",
  "tha-ap379:0.2_0": "Bodhivandanavagga",
  "tha-ap379:0.3_0": "7. Citakapūjakattheraapadāna",
  "tha-ap379:1.1_0": "“Parinibbute bhagavati,",
  "tha-ap379:1.2_0": "jalajuttamanāmake;",
  "tha-ap379:1.3_0": "Āropitamhi citake,",
  "tha-ap379:1.4_0": "sālapupphamapūjayiṁ.",
  "tha-ap379:2.1_0": "Satasahassito kappe,",
  "tha-ap379:2.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap379:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap379:2.4_0": "citapūjāyidaṁ phalaṁ.",
  "tha-ap379:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap379:3.2_0": "…pe…",
  "tha-ap379:3.3_0": "viharāmi anāsavo.",
  "tha-ap379:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap379:4.2_0": "…pe…",
  "tha-ap379:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap379:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap379:5.2_0": "…pe…",
  "tha-ap379:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap379:6.1_0": "Itthaṁ sudaṁ āyasmā citakapūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap379:7.1_0": "Citakapūjakattherassāpadānaṁ sattamaṁ."
}