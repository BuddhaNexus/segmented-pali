{
  "tha-ap191:0.1_0": "Therāpadāna",
  "tha-ap191:0.2_0": "Kuṭajapupphiyavagga",
  "tha-ap191:0.3_0": "9. Ekacāriyattheraapadāna",
  "tha-ap191:1.1_0": "“Tāvatiṁsesu devesu,",
  "tha-ap191:1.2_0": "mahāghoso tadā ahu;",
  "tha-ap191:1.3_0": "Buddho ca loke nibbāti,",
  "tha-ap191:1.4_0": "mayañcamha sarāgino.",
  "tha-ap191:2.1_0": "Tesaṁ saṁvegajātānaṁ,",
  "tha-ap191:2.2_0": "sokasallasamaṅginaṁ;",
  "tha-ap191:2.3_0": "Sabalena upatthaddho,",
  "tha-ap191:2.4_0": "agamaṁ buddhasantikaṁ.",
  "tha-ap191:3.1_0": "Mandāravaṁ gahetvāna,",
  "tha-ap191:3.2_0": "saṅgīti abhinimmitaṁ;",
  "tha-ap191:3.3_0": "Parinibbutakālamhi,",
  "tha-ap191:3.4_0": "buddhassa abhiropayiṁ.",
  "tha-ap191:4.1_0": "Sabbe devānumodiṁsu,",
  "tha-ap191:4.2_0": "accharāyo ca me tadā;",
  "tha-ap191:4.3_0": "Kappānaṁ satasahassaṁ,",
  "tha-ap191:4.4_0": "duggatiṁ nupapajjahaṁ.",
  "tha-ap191:5.1_0": "Saṭṭhikappasahassamhi,",
  "tha-ap191:5.2_0": "ito soḷasa te janā;",
  "tha-ap191:5.3_0": "Mahāmallajanā nāma,",
  "tha-ap191:5.4_0": "cakkavattī mahabbalā.",
  "tha-ap191:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap191:6.2_0": "…pe…",
  "tha-ap191:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap191:7.1_0": "Itthaṁ sudaṁ āyasmā ekacāriyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap191:8.1_0": "Ekacāriyattherassāpadānaṁ navamaṁ."
}