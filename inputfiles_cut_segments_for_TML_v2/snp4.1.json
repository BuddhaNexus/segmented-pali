{
  "snp4.1:0.1_0": "Sutta Nipāta 4.1",
  "snp4.1:0.2_0": "Kāmasutta",
  "snp4.1:1.1_0": "Kāmaṁ kāmayamānassa,",
  "snp4.1:1.2_0": "tassa ce taṁ samijjhati;",
  "snp4.1:1.3_0": "Addhā pītimano hoti,",
  "snp4.1:1.4_0": "laddhā macco yadicchati.",
  "snp4.1:2.1_0": "Tassa ce kāmayānassa,",
  "snp4.1:2.2_0": "chandajātassa jantuno;",
  "snp4.1:2.3_0": "Te kāmā parihāyanti,",
  "snp4.1:2.4_0": "sallaviddhova ruppati.",
  "snp4.1:3.1_0": "Yo kāme parivajjeti,",
  "snp4.1:3.2_0": "sappasseva padā siro;",
  "snp4.1:3.3_0": "Somaṁ visattikaṁ loke,",
  "snp4.1:3.4_0": "sato samativattati.",
  "snp4.1:4.1_0": "Khettaṁ vatthuṁ hiraññaṁ vā,",
  "snp4.1:4.2_0": "gavāssaṁ dāsaporisaṁ;",
  "snp4.1:4.3_0": "Thiyo bandhū puthu kāme,",
  "snp4.1:4.4_0": "yo naro anugijjhati.",
  "snp4.1:5.1_0": "Abalā naṁ balīyanti,",
  "snp4.1:5.2_0": "maddantenaṁ parissayā;",
  "snp4.1:5.3_0": "Tato naṁ dukkhamanveti,",
  "snp4.1:5.4_0": "nāvaṁ bhinnamivodakaṁ.",
  "snp4.1:6.1_0": "Tasmā jantu sadā sato,",
  "snp4.1:6.2_0": "Kāmāni parivajjaye;",
  "snp4.1:6.3_0": "Te pahāya tare oghaṁ,",
  "snp4.1:6.4_0": "Nāvaṁ sitvāva pāragūti.",
  "snp4.1:6.5_0": "Kāmasuttaṁ paṭhamaṁ."
}