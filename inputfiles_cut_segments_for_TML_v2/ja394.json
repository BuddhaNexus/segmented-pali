{
  "ja394:0.1_0": "Jātaka",
  "ja394:0.2_0": "Chakkanipāta",
  "ja394:0.3_0": "Kharaputtavagga",
  "ja394:0.4_0": "9. Vaṭṭakajātaka",
  "ja394:1.1_0": "“Paṇītaṁ bhuñjase bhattaṁ,",
  "ja394:1.2_0": "sappitelañca mātula;",
  "ja394:1.3_0": "Atha kena nu vaṇṇena,",
  "ja394:1.4_0": "kiso tvamasi vāyasa”.",
  "ja394:2.1_0": "“Amittamajjhe vasato,",
  "ja394:2.2_0": "tesu āmisamesato;",
  "ja394:2.3_0": "Niccaṁ ubbiggahadayassa,",
  "ja394:2.4_0": "kuto kākassa daḷhiyaṁ.",
  "ja394:3.1_0": "Niccaṁ ubbegino kākā,",
  "ja394:3.2_0": "dhaṅkā pāpena kammunā;",
  "ja394:3.3_0": "Laddho piṇḍo na pīṇeti,",
  "ja394:3.4_0": "kiso tenasmi vaṭṭaka.",
  "ja394:4.1_0": "Lūkhāni tiṇabījāni,",
  "ja394:4.2_0": "appasnehāni bhuñjasi;",
  "ja394:4.3_0": "Atha kena nu vaṇṇena,",
  "ja394:4.4_0": "thūlo tvamasi vaṭṭaka”.",
  "ja394:5.1_0": "“Appicchā appacintāya,",
  "ja394:5.2_0": "adūragamanena ca;",
  "ja394:5.3_0": "Laddhāladdhena yāpento,",
  "ja394:5.4_0": "thūlo tenasmi vāyasa.",
  "ja394:6.1_0": "Appicchassa hi posassa,",
  "ja394:6.2_0": "appacintasukhassa ca;",
  "ja394:6.3_0": "Susaṅgahitamānassa,",
  "ja394:6.4_0": "vuttī susamudānayā”ti.",
  "ja394:7.1_0": "Vaṭṭakajātakaṁ navamaṁ."
}