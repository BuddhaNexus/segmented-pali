{
  "ja330:0.1_0": "Jātaka",
  "ja330:0.2_0": "Catukkanipāta",
  "ja330:0.3_0": "Kuṭidūsakavagga",
  "ja330:0.4_0": "10. Sīlavīmaṁsajātaka",
  "ja330:1.1_0": "“Sīlaṁ kireva kalyāṇaṁ,",
  "ja330:1.2_0": "sīlaṁ loke anuttaraṁ;",
  "ja330:1.3_0": "Passa ghoraviso nāgo,",
  "ja330:1.4_0": "sīlavāti na haññati”.",
  "ja330:2.1_0": "“Yāvadevassahū kiñci,",
  "ja330:2.2_0": "tāvadeva akhādisuṁ;",
  "ja330:2.3_0": "Saṅgamma kulalā loke,",
  "ja330:2.4_0": "na hiṁsanti akiñcanaṁ.",
  "ja330:3.1_0": "Sukhaṁ nirāsā supati,",
  "ja330:3.2_0": "āsā phalavatī sukhā;",
  "ja330:3.3_0": "Āsaṁ nirāsaṁ katvāna,",
  "ja330:3.4_0": "sukhaṁ supati piṅgalā.",
  "ja330:4.1_0": "Na samādhiparo atthi,",
  "ja330:4.2_0": "asmiṁ loke paramhi ca;",
  "ja330:4.3_0": "Na paraṁ nāpi attānaṁ,",
  "ja330:4.4_0": "vihiṁsati samāhito”ti.",
  "ja330:5.1_0": "Sīlavīmaṁsajātakaṁ dasamaṁ.",
  "ja330:5.2_0": "Kuṭidūsakavaggo tatiyo.",
  "ja330:6.1_0": "Tassuddānaṁ",
  "ja330:7.1_0": "Samanussa saduddubha yācanako,",
  "ja330:7.2_0": "Atha meṇḍavaruttama godhavaro;",
  "ja330:7.3_0": "Atha kāyasakepuka bhotīvaro,",
  "ja330:7.4_0": "Atha rādhasusīlavarena dasāti."
}