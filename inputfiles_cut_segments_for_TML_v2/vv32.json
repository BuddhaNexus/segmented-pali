{
  "vv32:0.1_0": "Vimānavatthu",
  "vv32:0.2_0": "Itthivimāna",
  "vv32:0.3_0": "Pāricchattakavagga",
  "vv32:0.4_0": "4. Latāvimānavatthu",
  "vv32:1.1_0": "Latā ca sajjā pavarā ca devatā,",
  "vv32:1.2_0": "Accimatī rājavarassa sirīmato;",
  "vv32:1.3_0": "Sutā ca rañño vessavaṇassa dhītā,",
  "vv32:1.4_0": "Rājīmatī dhammaguṇehi sobhatha.",
  "vv32:2.1_0": "Pañcettha nāriyo āgamaṁsu nhāyituṁ,",
  "vv32:2.2_0": "Sītodakaṁ uppaliniṁ sivaṁ nadiṁ;",
  "vv32:2.3_0": "Tā tattha nhāyitvā rametvā devatā,",
  "vv32:2.4_0": "Naccitvā gāyitvā sutā lataṁ bravi.",
  "vv32:3.1_0": "“Pucchāmi taṁ uppalamāladhārini,",
  "vv32:3.2_0": "Āveḷini kañcanasannibhattace;",
  "vv32:3.3_0": "Timiratambakkhi nabheva sobhane,",
  "vv32:3.4_0": "Dīghāyukī kena kato yaso tava.",
  "vv32:4.1_0": "Kenāsi bhadde patino piyatarā,",
  "vv32:4.2_0": "Visiṭṭhakalyāṇitarassu rūpato;",
  "vv32:4.3_0": "Padakkhiṇā naccanagītavādite,",
  "vv32:4.4_0": "Ācikkha no tvaṁ naranāripucchitā”ti.",
  "vv32:5.1_0": "“Ahaṁ manussesu manussabhūtā,",
  "vv32:5.2_0": "Uḷārabhoge kule suṇisā ahosiṁ;",
  "vv32:5.3_0": "Akkodhanā bhattuvasānuvattinī,",
  "vv32:5.4_0": "Uposathe appamattā ahosiṁ.",
  "vv32:6.1_0": "Manussabhūtā daharā apāpikā,",
  "vv32:6.2_0": "Pasannacittā patimābhirādhayiṁ;",
  "vv32:6.3_0": "Sadevaraṁ sassasuraṁ sadāsakaṁ,",
  "vv32:6.4_0": "Abhirādhayiṁ tamhi kato yaso mama.",
  "vv32:7.1_0": "Sāhaṁ tena kusalena kammunā,",
  "vv32:7.2_0": "Catubbhi ṭhānehi visesamajjhagā;",
  "vv32:7.3_0": "Āyuñca vaṇṇañca sukhaṁ balañca,",
  "vv32:7.4_0": "Khiḍḍāratiṁ paccanubhomanappakaṁ”.",
  "vv32:8.1_0": "“Sutaṁ nu taṁ bhāsati yaṁ ayaṁ latā,",
  "vv32:8.2_0": "Yaṁ no apucchimha akittayī no;",
  "vv32:8.3_0": "Patino kiramhākaṁ visiṭṭha nārīnaṁ,",
  "vv32:8.4_0": "Gatī ca tāsaṁ pavarā ca devatā.",
  "vv32:9.1_0": "Patīsu dhammaṁ pacarāma sabbā,",
  "vv32:9.2_0": "Patibbatā yattha bhavanti itthiyo;",
  "vv32:9.3_0": "Patīsu dhammaṁ pacaritva sabbā,",
  "vv32:9.4_0": "Lacchāmase bhāsati yaṁ ayaṁ latā.",
  "vv32:10.1_0": "Sīho yathā pabbatasānugocaro,",
  "vv32:10.2_0": "Mahindharaṁ pabbatamāvasitvā;",
  "vv32:10.3_0": "Pasayha hantvā itare catuppade,",
  "vv32:10.4_0": "Khudde mige khādati maṁsabhojano.",
  "vv32:11.1_0": "Tatheva saddhā idha ariyasāvikā,",
  "vv32:11.2_0": "Bhattāraṁ nissāya patiṁ anubbatā;",
  "vv32:11.3_0": "Kodhaṁ vadhitvā abhibhuyya maccharaṁ,",
  "vv32:11.4_0": "Saggamhi sā modati dhammacārinī”ti.",
  "vv32:12.1_0": "Latāvimānaṁ catutthaṁ."
}