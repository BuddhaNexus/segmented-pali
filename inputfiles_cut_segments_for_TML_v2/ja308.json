{
  "ja308:0.1_0": "Jātaka",
  "ja308:0.2_0": "Catukkanipāta",
  "ja308:0.3_0": "Kāliṅgavagga",
  "ja308:0.4_0": "8. Sakuṇajātaka",
  "ja308:1.1_0": "“Akaramhasa te kiccaṁ,",
  "ja308:1.2_0": "yaṁ balaṁ ahuvamhase;",
  "ja308:1.3_0": "Migarāja namo tyatthu,",
  "ja308:1.4_0": "api kiñci labhāmase”.",
  "ja308:2.1_0": "“Mama lohitabhakkhassa,",
  "ja308:2.2_0": "niccaṁ luddāni kubbato;",
  "ja308:2.3_0": "Dantantaragato santo,",
  "ja308:2.4_0": "taṁ bahuṁ yampi jīvasi”.",
  "ja308:3.1_0": "“Akataññumakattāraṁ,",
  "ja308:3.2_0": "katassa appaṭikārakaṁ;",
  "ja308:3.3_0": "Yasmiṁ kataññutā natthi,",
  "ja308:3.4_0": "niratthā tassa sevanā.",
  "ja308:4.1_0": "Yassa sammukhaciṇṇena,",
  "ja308:4.2_0": "Mittadhammo na labbhati;",
  "ja308:4.3_0": "Anusūyamanakkosaṁ,",
  "ja308:4.4_0": "Saṇikaṁ tamhā apakkame”ti.",
  "ja308:5.1_0": "Sakuṇajātakaṁ aṭṭhamaṁ."
}