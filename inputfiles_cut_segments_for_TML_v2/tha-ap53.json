{
  "tha-ap53:0.1_0": "Therāpadāna",
  "tha-ap53:0.2_0": "Bījanivagga",
  "tha-ap53:0.3_0": "1. Vidhūpanadāyakattheraapadāna",
  "tha-ap53:1.1_0": "“Padumuttarabuddhassa,",
  "tha-ap53:1.2_0": "lokajeṭṭhassa tādino;",
  "tha-ap53:1.3_0": "Bījanikā mayā dinnā,",
  "tha-ap53:1.4_0": "dvipadindassa tādino.",
  "tha-ap53:2.1_0": "Sakaṁ cittaṁ pasādetvā,",
  "tha-ap53:2.2_0": "paggahetvāna añjaliṁ;",
  "tha-ap53:2.3_0": "Sambuddhamabhivādetvā,",
  "tha-ap53:2.4_0": "pakkamiṁ uttarāmukho.",
  "tha-ap53:3.1_0": "Bījaniṁ paggahetvāna,",
  "tha-ap53:3.2_0": "satthā lokagganāyako;",
  "tha-ap53:3.3_0": "Bhikkhusaṅghe ṭhito santo,",
  "tha-ap53:3.4_0": "imā gāthā abhāsatha.",
  "tha-ap53:4.1_0": "‘Iminā bījanidānena,",
  "tha-ap53:4.2_0": "cittassa paṇidhīhi ca;",
  "tha-ap53:4.3_0": "Kappānaṁ satasahassaṁ,",
  "tha-ap53:4.4_0": "vinipātaṁ na gacchati’.",
  "tha-ap53:5.1_0": "Āraddhavīriyo pahitatto,",
  "tha-ap53:5.2_0": "Cetoguṇasamāhito;",
  "tha-ap53:5.3_0": "Jātiyā sattavassohaṁ,",
  "tha-ap53:5.4_0": "Arahattaṁ apāpuṇiṁ.",
  "tha-ap53:6.1_0": "Saṭṭhikappasahassamhi,",
  "tha-ap53:6.2_0": "bījamānasanāmakā;",
  "tha-ap53:6.3_0": "Soḷasāsiṁsu rājāno,",
  "tha-ap53:6.4_0": "cakkavattī mahabbalā.",
  "tha-ap53:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap53:7.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap53:7.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap53:7.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap53:8.1_0": "Itthaṁ sudaṁ āyasmā vidhūpanadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap53:9.1_0": "Vidhūpanadāyakattherassāpadānaṁ paṭhamaṁ."
}