{
  "ja481:0.1_0": "Jātaka",
  "ja481:0.2_0": "Terasakanipāta",
  "ja481:0.3_0": "Ambavagga",
  "ja481:0.4_0": "8. Takkāriyajātaka",
  "ja481:1.1_0": "“Ahameva dubbhāsitaṁ bhāsi bālo,",
  "ja481:1.2_0": "Bhekovaraññe ahimavhāyamāno;",
  "ja481:1.3_0": "Takkāriye sobbhamimaṁ patāmi,",
  "ja481:1.4_0": "Na kireva sādhu ativelabhāṇī”.",
  "ja481:2.1_0": "“Pappoti macco ativelabhāṇī,",
  "ja481:2.2_0": "Bandhaṁ vadhaṁ sokapariddavañca;",
  "ja481:2.3_0": "Attānameva garahāsi ettha,",
  "ja481:2.4_0": "Ācera yaṁ taṁ nikhaṇanti sobbhe”.",
  "ja481:3.1_0": "“Kimevahaṁ tuṇḍilamanupucchiṁ,",
  "ja481:3.2_0": "Kareyya saṁ bhātaraṁ kāḷikāyaṁ;",
  "ja481:3.3_0": "Naggovahaṁ vatthayugañca jīno,",
  "ja481:3.4_0": "Ayampi attho bahutādisova”.",
  "ja481:4.1_0": "“Yo yujjhamānānamayujjhamāno,",
  "ja481:4.2_0": "Meṇḍantaraṁ accupatī kuliṅgo;",
  "ja481:4.3_0": "So piṁsito meṇḍasirehi tattha,",
  "ja481:4.4_0": "Ayampi attho bahutādisova.",
  "ja481:5.1_0": "Caturo janā potthakamaggahesuṁ,",
  "ja481:5.2_0": "Ekañca posaṁ anurakkhamānā;",
  "ja481:5.3_0": "Sabbeva te bhinnasirā sayiṁsu,",
  "ja481:5.4_0": "Ayampi attho bahutādisova.",
  "ja481:6.1_0": "Ajā yathā veḷugumbasmiṁ baddhā,",
  "ja481:6.2_0": "Avakkhipantī asimajjhagacchi;",
  "ja481:6.3_0": "Teneva tassā galakāvakantaṁ,",
  "ja481:6.4_0": "Ayampi attho bahutādisova”.",
  "ja481:7.1_0": "“Ime na devā na gandhabbaputtā,",
  "ja481:7.2_0": "Migā ime atthavasaṁ gatā me;",
  "ja481:7.3_0": "Ekañca naṁ sāyamāse pacantu,",
  "ja481:7.4_0": "Ekaṁ punappātarāse pacantu”.",
  "ja481:8.1_0": "“Sataṁ sahassāni dubhāsitāni,",
  "ja481:8.2_0": "Kalampi nāgghanti subhāsitassa;",
  "ja481:8.3_0": "Dubbhāsitaṁ saṅkamāno kileso,",
  "ja481:8.4_0": "Tasmā tuṇhī kimpurisā na bālyā”.",
  "ja481:9.1_0": "“Yā mesā byāhāsi pamuñcathetaṁ,",
  "ja481:9.2_0": "Giriñca naṁ himavantaṁ nayantu;",
  "ja481:9.3_0": "Imañca kho dentu mahānasāya,",
  "ja481:9.4_0": "Pātova naṁ pātarāse pacantu”.",
  "ja481:10.1_0": "“Pajjunnanāthā pasavo,",
  "ja481:10.2_0": "pasunāthā ayaṁ pajā;",
  "ja481:10.3_0": "Tvaṁ nāthosi mahārāja,",
  "ja481:10.4_0": "nāthohaṁ bhariyāya me;",
  "ja481:10.5_0": "Dvinnamaññataraṁ ñatvā,",
  "ja481:10.6_0": "mutto gaccheyya pabbataṁ.",
  "ja481:11.1_0": "Na ve nindā suparivajjayetha,",
  "ja481:11.2_0": "Nānā janā sevitabbā janinda;",
  "ja481:11.3_0": "Yeneva eko labhate pasaṁsaṁ,",
  "ja481:11.4_0": "Teneva añño labhate ninditāraṁ.",
  "ja481:12.1_0": "Sabbo loko paricitto aticitto,",
  "ja481:12.2_0": "Sabbo loko cittavā samhi citte;",
  "ja481:12.3_0": "Paccekacittā puthu sabbasattā,",
  "ja481:12.4_0": "Kassīdha cittassa vasena vatte”.",
  "ja481:13.1_0": "“Tuṇhī ahū kimpuriso sabhariyo,",
  "ja481:13.2_0": "Yo dāni byāhāsi bhayassa bhīto;",
  "ja481:13.3_0": "So dāni mutto sukhito arogo,",
  "ja481:13.4_0": "Vācākirevatthavatī narānan”ti.",
  "ja481:14.1_0": "Takkāriyajātakaṁ aṭṭhamaṁ."
}