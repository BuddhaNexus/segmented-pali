{
  "thag1.51:0.1_0": "Theragāthā",
  "thag1.51:0.2_0": "Ekakanipāta",
  "thag1.51:0.3_0": "Chaṭṭhavagga",
  "thag1.51:0.4_0": "1.51. Godhikattheragāthā",
  "thag1.51:1.1_0": "“Vassati devo yathā sugītaṁ,",
  "thag1.51:1.2_0": "Channā me kuṭikā sukhā nivātā;",
  "thag1.51:1.3_0": "Cittaṁ susamāhitañca mayhaṁ,",
  "thag1.51:1.4_0": "Atha ce patthayasi pavassa devā”ti.",
  "thag1.51:1.5_0": "… Godhiko thero …."
}