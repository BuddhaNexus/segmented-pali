{
  "snp2.6:0.1_0": "Sutta Nipāta 2.6",
  "snp2.6:0.2_0": "Kapilasutta (dhammacariyasutta)",
  "snp2.6:1.1_0": "Dhammacariyaṁ brahmacariyaṁ,",
  "snp2.6:1.2_0": "Etadāhu vasuttamaṁ;",
  "snp2.6:1.3_0": "Pabbajitopi ce hoti,",
  "snp2.6:1.4_0": "Agārā anagāriyaṁ.",
  "snp2.6:2.1_0": "So ce mukharajātiko,",
  "snp2.6:2.2_0": "vihesābhirato mago;",
  "snp2.6:2.3_0": "Jīvitaṁ tassa pāpiyo,",
  "snp2.6:2.4_0": "rajaṁ vaḍḍheti attano.",
  "snp2.6:3.1_0": "Kalahābhirato bhikkhu,",
  "snp2.6:3.2_0": "Mohadhammena āvuto;",
  "snp2.6:3.3_0": "Akkhātampi na jānāti,",
  "snp2.6:3.4_0": "Dhammaṁ buddhena desitaṁ.",
  "snp2.6:4.1_0": "Vihesaṁ bhāvitattānaṁ,",
  "snp2.6:4.2_0": "avijjāya purakkhato;",
  "snp2.6:4.3_0": "Saṅkilesaṁ na jānāti,",
  "snp2.6:4.4_0": "maggaṁ nirayagāminaṁ.",
  "snp2.6:5.1_0": "Vinipātaṁ samāpanno,",
  "snp2.6:5.2_0": "gabbhā gabbhaṁ tamā tamaṁ;",
  "snp2.6:5.3_0": "Sa ve tādisako bhikkhu,",
  "snp2.6:5.4_0": "pecca dukkhaṁ nigacchati.",
  "snp2.6:6.1_0": "Gūthakūpo yathā assa,",
  "snp2.6:6.2_0": "sampuṇṇo gaṇavassiko;",
  "snp2.6:6.3_0": "Yo ca evarūpo assa,",
  "snp2.6:6.4_0": "dubbisodho hi sāṅgaṇo.",
  "snp2.6:7.1_0": "Yaṁ evarūpaṁ jānātha,",
  "snp2.6:7.2_0": "bhikkhavo gehanissitaṁ;",
  "snp2.6:7.3_0": "Pāpicchaṁ pāpasaṅkappaṁ,",
  "snp2.6:7.4_0": "pāpaācāragocaraṁ.",
  "snp2.6:8.1_0": "Sabbe samaggā hutvāna,",
  "snp2.6:8.2_0": "abhinibbajjiyātha naṁ;",
  "snp2.6:8.3_0": "Kāraṇḍavaṁ niddhamatha,",
  "snp2.6:8.4_0": "kasambuṁ apakassatha.",
  "snp2.6:9.1_0": "Tato palāpe vāhetha,",
  "snp2.6:9.2_0": "Assamaṇe samaṇamānine;",
  "snp2.6:9.3_0": "Niddhamitvāna pāpicche,",
  "snp2.6:9.4_0": "Pāpaācāragocare.",
  "snp2.6:10.1_0": "Suddhā suddhehi saṁvāsaṁ,",
  "snp2.6:10.2_0": "Kappayavho patissatā;",
  "snp2.6:10.3_0": "Tato samaggā nipakā,",
  "snp2.6:10.4_0": "Dukkhassantaṁ karissathāti.",
  "snp2.6:10.5_0": "Dhammacariyasuttaṁ chaṭṭhaṁ."
}