{
  "vv68:0.1_0": "Vimānavatthu",
  "vv68:0.2_0": "Purisavimāna",
  "vv68:0.3_0": "Pāyāsivagga",
  "vv68:0.4_0": "4. Paṭhamaupassayadāyakavimānavatthu",
  "vv68:1.1_0": "“Cando yathā vigatavalāhake nabhe,",
  "vv68:1.2_0": "Obhāsayaṁ gacchati antalikkhe;",
  "vv68:1.3_0": "Tathūpamaṁ tuyhamidaṁ vimānaṁ,",
  "vv68:1.4_0": "Obhāsayaṁ tiṭṭhati antalikkhe.",
  "vv68:2.1_0": "Deviddhipattosi mahānubhāvo,",
  "vv68:2.2_0": "Manussabhūto kimakāsi puññaṁ;",
  "vv68:2.3_0": "Kenāsi evaṁ jalitānubhāvo,",
  "vv68:2.4_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv68:3.1_0": "So devaputto attamano,",
  "vv68:3.2_0": "…pe…",
  "vv68:3.3_0": "yassa kammassidaṁ phalaṁ.",
  "vv68:4.1_0": "“Ahañca bhariyā ca manussaloke,",
  "vv68:4.2_0": "Upassayaṁ arahato adamha;",
  "vv68:4.3_0": "Annañca pānañca pasannacittā,",
  "vv68:4.4_0": "Sakkacca dānaṁ vipulaṁ adamha.",
  "vv68:5.1_0": "Tena metādiso vaṇṇo,",
  "vv68:5.2_0": "…pe…",
  "vv68:5.3_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv68:6.1_0": "Paṭhamaupassayadāyakavimānaṁ catutthaṁ."
}