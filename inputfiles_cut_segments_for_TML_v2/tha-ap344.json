{
  "tha-ap344:0.1_0": "Therāpadāna",
  "tha-ap344:0.2_0": "Ekapadumiyavagga",
  "tha-ap344:0.3_0": "2 Tīṇuppalamāliyattheraapadāna",
  "tha-ap344:1.1_0": "“Candabhāgānadītīre,",
  "tha-ap344:1.2_0": "ahosiṁ vānaro tadā;",
  "tha-ap344:1.3_0": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap344:1.4_0": "nisinnaṁ pabbatantare.",
  "tha-ap344:2.1_0": "Obhāsentaṁ disā sabbā,",
  "tha-ap344:2.2_0": "sālarājaṁva phullitaṁ;",
  "tha-ap344:2.3_0": "Lakkhaṇabyañjanūpetaṁ,",
  "tha-ap344:2.4_0": "disvā attamano ahuṁ.",
  "tha-ap344:3.1_0": "Udaggacitto sumano,",
  "tha-ap344:3.2_0": "pītiyā haṭṭhamānaso;",
  "tha-ap344:3.3_0": "Tīṇi uppalapupphāni,",
  "tha-ap344:3.4_0": "matthake abhiropayiṁ.",
  "tha-ap344:4.1_0": "Pupphāni abhiropetvā,",
  "tha-ap344:4.2_0": "vipassissa mahesino;",
  "tha-ap344:4.3_0": "Sagāravo bhavitvāna,",
  "tha-ap344:4.4_0": "pakkāmiṁ uttarāmukho.",
  "tha-ap344:5.1_0": "Gacchanto paṭikuṭiko,",
  "tha-ap344:5.2_0": "vippasannena cetasā;",
  "tha-ap344:5.3_0": "Selantare patitvāna,",
  "tha-ap344:5.4_0": "pāpuṇiṁ jīvitakkhayaṁ.",
  "tha-ap344:6.1_0": "Tena kammena sukatena,",
  "tha-ap344:6.2_0": "cetanāpaṇidhīhi ca;",
  "tha-ap344:6.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "tha-ap344:6.4_0": "tāvatiṁsamagacchahaṁ.",
  "tha-ap344:7.1_0": "Satānaṁ tīṇikkhattuñca,",
  "tha-ap344:7.2_0": "devarajjaṁ akārayiṁ;",
  "tha-ap344:7.3_0": "Satānaṁ pañcakkhattuñca,",
  "tha-ap344:7.4_0": "cakkavattī ahosahaṁ.",
  "tha-ap344:8.1_0": "Ekanavutito kappe,",
  "tha-ap344:8.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap344:8.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap344:8.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap344:9.1_0": "Paṭisambhidā catasso,",
  "tha-ap344:9.2_0": "…pe…",
  "tha-ap344:9.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap344:10.1_0": "Itthaṁ sudaṁ āyasmā tīṇuppalamāliyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap344:11.1_0": "Tīṇuppalamāliyattherassāpadānaṁ dutiyaṁ."
}