{
  "ja344:0.1_0": "Jātaka",
  "ja344:0.2_0": "Catukkanipāta",
  "ja344:0.3_0": "Cūḷakuṇālavagga",
  "ja344:0.4_0": "4. Ambajātaka",
  "ja344:1.1_0": "“Yo nīliyaṁ maṇḍayati,",
  "ja344:1.2_0": "saṇḍāsena vihaññati;",
  "ja344:1.3_0": "Tassa sā vasamanvetu,",
  "ja344:1.4_0": "yā te ambe avāhari”.",
  "ja344:2.1_0": "“Vīsaṁ vā pañcavīsaṁ vā,",
  "ja344:2.2_0": "ūnatiṁsaṁ va jātiyā;",
  "ja344:2.3_0": "Tādisā pati mā laddhā,",
  "ja344:2.4_0": "yā te ambe avāhari”.",
  "ja344:3.1_0": "“Dīghaṁ gacchatu addhānaṁ,",
  "ja344:3.2_0": "ekikā abhisārikā;",
  "ja344:3.3_0": "Saṅkete pati mā adda,",
  "ja344:3.4_0": "yā te ambe avāhari”.",
  "ja344:4.1_0": "“Alaṅkatā suvasanā,",
  "ja344:4.2_0": "mālinī candanussadā;",
  "ja344:4.3_0": "Ekikā sayane setu,",
  "ja344:4.4_0": "yā te ambe avāharī”ti.",
  "ja344:5.1_0": "Ambajātakaṁ catutthaṁ."
}