{
  "ja435:0.1_0": "Jātaka",
  "ja435:0.2_0": "Navakanipāta",
  "ja435:0.3_0": "Gijjhavagga",
  "ja435:0.4_0": "9. Haliddirāgajātaka",
  "ja435:1.1_0": "“Sutitikkhaṁ araññamhi,",
  "ja435:1.2_0": "pantamhi sayanāsane;",
  "ja435:1.3_0": "Ye ca gāme titikkhanti,",
  "ja435:1.4_0": "te uḷāratarā tayā”.",
  "ja435:2.1_0": "“Araññā gāmamāgamma,",
  "ja435:2.2_0": "kiṁsīlaṁ kiṁvataṁ ahaṁ;",
  "ja435:2.3_0": "Purisaṁ tāta seveyyaṁ,",
  "ja435:2.4_0": "taṁ me akkhāhi pucchito”.",
  "ja435:3.1_0": "“Yo te vissāsaye tāta,",
  "ja435:3.2_0": "vissāsañca khameyya te;",
  "ja435:3.3_0": "Sussūsī ca titikkhī ca,",
  "ja435:3.4_0": "taṁ bhajehi ito gato.",
  "ja435:4.1_0": "Yassa kāyena vācāya,",
  "ja435:4.2_0": "manasā natthi dukkaṭaṁ;",
  "ja435:4.3_0": "Urasīva patiṭṭhāya,",
  "ja435:4.4_0": "taṁ bhajehi ito gato.",
  "ja435:5.1_0": "Yo ca dhammena carati,",
  "ja435:5.2_0": "carantopi na maññati;",
  "ja435:5.3_0": "Visuddhakāriṁ sappaññaṁ,",
  "ja435:5.4_0": "taṁ bhajehi ito gato.",
  "ja435:6.1_0": "Haliddirāgaṁ kapicittaṁ,",
  "ja435:6.2_0": "purisaṁ rāgavirāginaṁ;",
  "ja435:6.3_0": "Tādisaṁ tāta mā sevi,",
  "ja435:6.4_0": "nimmanussampi ce siyā.",
  "ja435:7.1_0": "Āsīvisaṁva kupitaṁ,",
  "ja435:7.2_0": "mīḷhalittaṁ mahāpathaṁ;",
  "ja435:7.3_0": "Ārakā parivajjehi,",
  "ja435:7.4_0": "yānīva visamaṁ pathaṁ.",
  "ja435:8.1_0": "Anatthā tāta vaḍḍhanti,",
  "ja435:8.2_0": "bālaṁ accupasevato;",
  "ja435:8.3_0": "Māssu bālena saṅgacchi,",
  "ja435:8.4_0": "amitteneva sabbadā.",
  "ja435:9.1_0": "Taṁ tāhaṁ tāta yācāmi,",
  "ja435:9.2_0": "karassu vacanaṁ mama;",
  "ja435:9.3_0": "Māssu bālena saṅgacchi,",
  "ja435:9.4_0": "dukkho bālehi saṅgamo”ti.",
  "ja435:10.1_0": "Haliddirāgajātakaṁ navamaṁ."
}