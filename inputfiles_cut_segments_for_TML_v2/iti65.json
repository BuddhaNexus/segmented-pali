{
  "iti65:0.1_0": "Itivuttaka 65",
  "iti65:0.2_0": "Tikanipāta",
  "iti65:0.3_0": "Dutiyavagga",
  "iti65:0.4_0": "Sucaritasutta",
  "iti65:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti65:2.1_0": "“Tīṇimāni, bhikkhave, sucaritāni.",
  "iti65:2.2_0": "Katamāni tīṇi?",
  "iti65:2.3_0": "Kāyasucaritaṁ, vacīsucaritaṁ, manosucaritaṁ—",
  "iti65:2.4_0": "imāni kho, bhikkhave, tīṇi sucaritānī”ti.",
  "iti65:2.5_0": "Etamatthaṁ bhagavā avoca.",
  "iti65:2.6_0": "Tatthetaṁ iti vuccati:",
  "iti65:3.1_0": "“Kāyaduccaritaṁ hitvā,",
  "iti65:3.2_0": "vacīduccaritāni ca;",
  "iti65:3.3_0": "Manoduccaritaṁ hitvā,",
  "iti65:3.4_0": "yañcaññaṁ dosasaṁhitaṁ.",
  "iti65:4.1_0": "Akatvākusalaṁ kammaṁ,",
  "iti65:4.2_0": "katvāna kusalaṁ bahuṁ;",
  "iti65:4.3_0": "Kāyassa bhedā sappañño,",
  "iti65:4.4_0": "saggaṁ so upapajjatī”ti.",
  "iti65:5.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti65:5.2_0": "Chaṭṭhaṁ."
}