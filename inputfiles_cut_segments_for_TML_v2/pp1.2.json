{
  "pp1.2:0.1_0": "Puggalapaññatti",
  "pp1.2:0.2_0": "Mātikā",
  "pp1.2:0.3_0": "2. Dukauddesa",
  "pp1.2:1.1_0": "**Dve puggalā—**",
  "pp1.2:2.1_0": "Kodhano ca, upanāhī ca.",
  "pp1.2:3.1_0": "Makkhī ca, paḷāsī ca.",
  "pp1.2:4.1_0": "Issukī ca, maccharī ca.",
  "pp1.2:5.1_0": "Saṭho ca, māyāvī ca.",
  "pp1.2:6.1_0": "Ahiriko ca, anottappī ca.",
  "pp1.2:7.1_0": "Dubbaco ca, pāpamitto ca.",
  "pp1.2:8.1_0": "Indriyesu aguttadvāro ca, bhojane amattaññū ca.",
  "pp1.2:9.1_0": "Muṭṭhassati ca, asampajāno ca.",
  "pp1.2:10.1_0": "Sīlavipanno ca, diṭṭhivipanno ca.",
  "pp1.2:11.1_0": "Ajjhattasaṁyojano ca, bahiddhāsaṁyojano ca.",
  "pp1.2:12.1_0": "Akkodhano ca, anupanāhī ca.",
  "pp1.2:13.1_0": "Amakkhī ca, apaḷāsī ca.",
  "pp1.2:14.1_0": "Anissukī ca, amaccharī ca.",
  "pp1.2:15.1_0": "Asaṭho ca, amāyāvī ca.",
  "pp1.2:16.1_0": "Hirimā ca, ottappī ca.",
  "pp1.2:17.1_0": "Suvaco ca, kalyāṇamitto ca.",
  "pp1.2:18.1_0": "Indriyesu guttadvāro ca, bhojane mattaññū ca.",
  "pp1.2:19.1_0": "Upaṭṭhitassati ca, sampajāno ca.",
  "pp1.2:20.1_0": "Sīlasampanno ca, diṭṭhisampanno ca.",
  "pp1.2:21.1_0": "Dve puggalā dullabhā lokasmiṁ.",
  "pp1.2:22.1_0": "Dve puggalā duttappayā.",
  "pp1.2:23.1_0": "Dve puggalā sutappayā.",
  "pp1.2:24.1_0": "Dvinnaṁ puggalānaṁ āsavā vaḍḍhanti.",
  "pp1.2:25.1_0": "Dvinnaṁ puggalānaṁ āsavā na vaḍḍhanti.",
  "pp1.2:26.1_0": "Hīnādhimutto ca, paṇītādhimutto ca.",
  "pp1.2:27.1_0": "Titto ca, tappetā ca.",
  "pp1.2:28.1_0": "Dukaṁ."
}