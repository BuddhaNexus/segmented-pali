{
  "ja477:0.1_0": "Jātaka",
  "ja477:0.2_0": "Terasakanipāta",
  "ja477:0.3_0": "Ambavagga",
  "ja477:0.4_0": "4. Cūḷanāradajātaka",
  "ja477:1.1_0": "“Na te kaṭṭhāni bhinnāni,",
  "ja477:1.2_0": "na te udakamābhataṁ;",
  "ja477:1.3_0": "Aggīpi te na hāpito,",
  "ja477:1.4_0": "kiṁ nu mandova jhāyasi”.",
  "ja477:2.1_0": "“Na ussahe vane vatthuṁ,",
  "ja477:2.2_0": "kassapāmantayāmi taṁ;",
  "ja477:2.3_0": "Dukkho vāso araññasmiṁ,",
  "ja477:2.4_0": "raṭṭhaṁ icchāmi gantave.",
  "ja477:3.1_0": "Yathā ahaṁ ito gantvā,",
  "ja477:3.2_0": "Yasmiṁ janapade vasaṁ;",
  "ja477:3.3_0": "Ācāraṁ brahme sikkheyyaṁ,",
  "ja477:3.4_0": "Taṁ dhammaṁ anusāsa maṁ”.",
  "ja477:4.1_0": "“Sace araññaṁ hitvāna,",
  "ja477:4.2_0": "vanamūlaphalāni ca;",
  "ja477:4.3_0": "Raṭṭhe rocayase vāsaṁ,",
  "ja477:4.4_0": "taṁ dhammaṁ nisāmehi me.",
  "ja477:5.1_0": "Visaṁ mā paṭisevittho,",
  "ja477:5.2_0": "papātaṁ parivajjaya;",
  "ja477:5.3_0": "Paṅke ca mā visīdittho,",
  "ja477:5.4_0": "yatto cāsīvise care”.",
  "ja477:6.1_0": "“Kiṁ nu visaṁ papāto vā,",
  "ja477:6.2_0": "paṅko vā brahmacārinaṁ;",
  "ja477:6.3_0": "Kaṁ tvaṁ āsīvisaṁ brūsi,",
  "ja477:6.4_0": "taṁ me akkhāhi pucchito”.",
  "ja477:7.1_0": "“Āsavo tāta lokasmiṁ,",
  "ja477:7.2_0": "surā nāma pavuccati;",
  "ja477:7.3_0": "Manuñño surabhī vaggu,",
  "ja477:7.4_0": "sādu khuddarasūpamo;",
  "ja477:7.5_0": "Visaṁ tadāhu ariyā se,",
  "ja477:7.6_0": "brahmacariyassa nārada.",
  "ja477:8.1_0": "Itthiyo tāta lokasmiṁ,",
  "ja477:8.2_0": "pamattaṁ pamathenti tā;",
  "ja477:8.3_0": "Haranti yuvino cittaṁ,",
  "ja477:8.4_0": "tūlaṁ bhaṭṭhaṁva māluto;",
  "ja477:8.5_0": "Papāto eso akkhāto,",
  "ja477:8.6_0": "brahmacariyassa nārada.",
  "ja477:9.1_0": "Lābho siloko sakkāro,",
  "ja477:9.2_0": "pūjā parakulesu ca;",
  "ja477:9.3_0": "Paṅko eso ca akkhāto,",
  "ja477:9.4_0": "brahmacariyassa nārada.",
  "ja477:10.1_0": "Sasatthā tāta rājāno,",
  "ja477:10.2_0": "āvasanti mahiṁ imaṁ;",
  "ja477:10.3_0": "Te tādise manussinde,",
  "ja477:10.4_0": "mahante tāta nārada.",
  "ja477:11.1_0": "Issarānaṁ adhipatīnaṁ,",
  "ja477:11.2_0": "na tesaṁ pādato care;",
  "ja477:11.3_0": "Āsīvisoti akkhāto,",
  "ja477:11.4_0": "brahmacariyassa nārada.",
  "ja477:12.1_0": "Bhattattho bhattakāle ca,",
  "ja477:12.2_0": "yaṁ gehamupasaṅkame;",
  "ja477:12.3_0": "Yadettha kusalaṁ jaññā,",
  "ja477:12.4_0": "tattha ghāsesanaṁ care.",
  "ja477:13.1_0": "Pavisitvā parakulaṁ,",
  "ja477:13.2_0": "pānatthaṁ bhojanāya vā;",
  "ja477:13.3_0": "Mitaṁ khāde mitaṁ bhuñje,",
  "ja477:13.4_0": "na ca rūpe manaṁ kare.",
  "ja477:14.1_0": "Goṭṭhaṁ majjaṁ kirāṭañca,",
  "ja477:14.2_0": "sabhā nikiraṇāni ca;",
  "ja477:14.3_0": "Ārakā parivajjehi,",
  "ja477:14.4_0": "yānīva visamaṁ pathan”ti.",
  "ja477:15.1_0": "Cūḷanāradajātakaṁ catutthaṁ."
}