{
  "tha-ap79:0.1_0": "Therāpadāna",
  "tha-ap79:0.2_0": "Nāgasamālavagga",
  "tha-ap79:0.3_0": "7. Sūcidāyakattheraapadāna",
  "tha-ap79:1.1_0": "“Tiṁsakappasahassamhi,",
  "tha-ap79:1.2_0": "sambuddho lokanāyako;",
  "tha-ap79:1.3_0": "Sumedho nāma nāmena,",
  "tha-ap79:1.4_0": "bāttiṁsavaralakkhaṇo.",
  "tha-ap79:2.1_0": "Tassa kañcanavaṇṇassa,",
  "tha-ap79:2.2_0": "dvipadindassa tādino;",
  "tha-ap79:2.3_0": "Pañca sūcī mayā dinnā,",
  "tha-ap79:2.4_0": "sibbanatthāya cīvaraṁ.",
  "tha-ap79:3.1_0": "Teneva sūcidānena,",
  "tha-ap79:3.2_0": "nipuṇatthavipassakaṁ;",
  "tha-ap79:3.3_0": "Tikkhaṁ lahuñca phāsuñca,",
  "tha-ap79:3.4_0": "ñāṇaṁ me udapajjatha.",
  "tha-ap79:4.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap79:4.2_0": "bhavā sabbe samūhatā;",
  "tha-ap79:4.3_0": "Dhāremi antimaṁ dehaṁ,",
  "tha-ap79:4.4_0": "sammāsambuddhasāsane.",
  "tha-ap79:5.1_0": "Dvipadādhipatī nāma,",
  "tha-ap79:5.2_0": "rājāno caturo ahuṁ;",
  "tha-ap79:5.3_0": "Sattaratanasampannā,",
  "tha-ap79:5.4_0": "cakkavattī mahabbalā.",
  "tha-ap79:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap79:6.2_0": "…pe…",
  "tha-ap79:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap79:7.1_0": "Itthaṁ sudaṁ āyasmā sūcidāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap79:8.1_0": "Sūcidāyakattherassāpadānaṁ sattamaṁ."
}