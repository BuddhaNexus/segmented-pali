{
  "thag2.48:0.1_0": "Theragāthā",
  "thag2.48:0.2_0": "Dukanipāta",
  "thag2.48:0.3_0": "Pañcamavagga",
  "thag2.48:0.4_0": "2.48. Vajjitattheragāthā",
  "thag2.48:1.1_0": "“Saṁsaraṁ dīghamaddhānaṁ,",
  "thag2.48:1.2_0": "gatīsu parivattisaṁ;",
  "thag2.48:1.3_0": "Apassaṁ ariyasaccāni,",
  "thag2.48:1.4_0": "andhabhūto puthujjano.",
  "thag2.48:2.1_0": "Tassa me appamattassa,",
  "thag2.48:2.2_0": "saṁsārā vinaḷīkatā;",
  "thag2.48:2.3_0": "Sabbā gatī samucchinnā,",
  "thag2.48:2.4_0": "natthi dāni punabbhavo”ti.",
  "thag2.48:2.5_0": "… Vajjito thero …."
}