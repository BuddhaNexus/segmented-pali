{
  "ja203:0.1_0": "Jātaka",
  "ja203:0.2_0": "Dukanipāta",
  "ja203:0.3_0": "Nataṁdaḷhavagga",
  "ja203:0.4_0": "3. Khaṇḍajātaka",
  "ja203:1.1_0": "“Virūpakkhehi me mettaṁ,",
  "ja203:1.2_0": "Mettaṁ erāpathehi me;",
  "ja203:1.3_0": "Chabyāputtehi me mettaṁ,",
  "ja203:1.4_0": "Mettaṁ kaṇhāgotamakehi ca.",
  "ja203:2.1_0": "Apādakehi me mettaṁ,",
  "ja203:2.2_0": "mettaṁ dvipādakehi me;",
  "ja203:2.3_0": "Catuppadehi me mettaṁ,",
  "ja203:2.4_0": "mettaṁ bahuppadehi me.",
  "ja203:3.1_0": "Mā maṁ apādako hiṁsi,",
  "ja203:3.2_0": "mā maṁ hiṁsi dvipādako;",
  "ja203:3.3_0": "Mā maṁ catuppado hiṁsi,",
  "ja203:3.4_0": "mā maṁ hiṁsi bahuppado.",
  "ja203:4.1_0": "Sabbe sattā sabbe pāṇā,",
  "ja203:4.2_0": "sabbe bhūtā ca kevalā;",
  "ja203:4.3_0": "Sabbe bhadrāni passantu,",
  "ja203:4.4_0": "mā kañci pāpamāgamā.",
  "ja203:5.1_0": "Appamāṇo buddho,",
  "ja203:5.2_0": "Appamāṇo dhammo;",
  "ja203:5.3_0": "Appamāṇo saṅgho,",
  "ja203:5.4_0": "Pamāṇavantāni sarīsapāni;",
  "ja203:5.5_0": "Ahivicchikasatapadī,",
  "ja203:5.6_0": "Uṇṇanābhi sarabū mūsikā.",
  "ja203:6.1_0": "Katā me rakkhā katā me parittā,",
  "ja203:6.2_0": "Paṭikkamantu bhūtāni;",
  "ja203:6.3_0": "Sohaṁ namo bhagavato,",
  "ja203:6.4_0": "Namo sattannaṁ sammāsambuddhānan”ti.",
  "ja203:7.1_0": "Khaṇḍajātakaṁ tatiyaṁ."
}