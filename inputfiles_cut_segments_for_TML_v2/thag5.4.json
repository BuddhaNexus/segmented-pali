{
  "thag5.4:0.1_0": "Theragāthā",
  "thag5.4:0.2_0": "Pañcakanipāta",
  "thag5.4:0.3_0": "Paṭhamavagga",
  "thag5.4:0.4_0": "5.4. Sumanattheragāthā",
  "thag5.4:1.1_0": "“Yaṁ patthayāno dhammesu,",
  "thag5.4:1.2_0": "upajjhāyo anuggahi;",
  "thag5.4:1.3_0": "Amataṁ abhikaṅkhantaṁ,",
  "thag5.4:1.4_0": "kataṁ kattabbakaṁ mayā.",
  "thag5.4:2.1_0": "Anuppatto sacchikato,",
  "thag5.4:2.2_0": "sayaṁ dhammo anītiho;",
  "thag5.4:2.3_0": "Visuddhañāṇo nikkaṅkho,",
  "thag5.4:2.4_0": "byākaromi tavantike.",
  "thag5.4:3.1_0": "Pubbenivāsaṁ jānāmi,",
  "thag5.4:3.2_0": "dibbacakkhu visodhitaṁ;",
  "thag5.4:3.3_0": "Sadattho me anuppatto,",
  "thag5.4:3.4_0": "kataṁ buddhassa sāsanaṁ.",
  "thag5.4:4.1_0": "Appamattassa me sikkhā,",
  "thag5.4:4.2_0": "sussutā tava sāsane;",
  "thag5.4:4.3_0": "Sabbe me āsavā khīṇā,",
  "thag5.4:4.4_0": "natthi dāni punabbhavo.",
  "thag5.4:5.1_0": "Anusāsi maṁ ariyavatā,",
  "thag5.4:5.2_0": "Anukampi anuggahi;",
  "thag5.4:5.3_0": "Amogho tuyhamovādo,",
  "thag5.4:5.4_0": "Antevāsimhi sikkhito”ti.",
  "thag5.4:5.5_0": "… Sumano thero …."
}