{
  "tha-ap524:0.1_0": "Therāpadāna",
  "tha-ap524:0.2_0": "Tiṇadāyakavagga",
  "tha-ap524:0.3_0": "2 Mañcadāyakattheraapadāna",
  "tha-ap524:1.1_0": "“Vipassino bhagavato,",
  "tha-ap524:1.2_0": "lokajeṭṭhassa tādino;",
  "tha-ap524:1.3_0": "Ekamañcaṁ mayā dinnaṁ,",
  "tha-ap524:1.4_0": "pasannena sapāṇinā.",
  "tha-ap524:2.1_0": "Hatthiyānaṁ assayānaṁ,",
  "tha-ap524:2.2_0": "dibbayānaṁ samajjhagaṁ;",
  "tha-ap524:2.3_0": "Tena mañcakadānena,",
  "tha-ap524:2.4_0": "pattomhi āsavakkhayaṁ.",
  "tha-ap524:3.1_0": "Ekanavutito kappe,",
  "tha-ap524:3.2_0": "yaṁ mañcamadadiṁ tadā;",
  "tha-ap524:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap524:3.4_0": "mañcadānassidaṁ phalaṁ.",
  "tha-ap524:4.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap524:4.2_0": "…pe…",
  "tha-ap524:4.3_0": "viharāmi anāsavo.",
  "tha-ap524:5.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap524:5.2_0": "…pe…",
  "tha-ap524:5.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap524:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap524:6.2_0": "…pe…",
  "tha-ap524:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap524:7.1_0": "Itthaṁ sudaṁ āyasmā mañcadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap524:8.1_0": "Mañcadāyakattherassāpadānaṁ dutiyaṁ."
}