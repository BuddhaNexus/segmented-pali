{
  "kv1.8:0.1_0": "Kathāvatthu",
  "kv1.8:0.2_0": "Mahāpaṇṇāsaka",
  "kv1.8:0.3_0": "Paṭhamavagga",
  "kv1.8:0.4_0": "Ekaccaṁatthītikathā",
  "kv1.8:1.0_0": "Atītādiekaccakathā",
  "kv1.8:1.1_0": "Atītaṁ atthīti?",
  "kv1.8:1.2_0": "Ekaccaṁ atthi, ekaccaṁ natthīti.",
  "kv1.8:1.3_0": "Ekaccaṁ niruddhaṁ, ekaccaṁ na niruddhaṁ;",
  "kv1.8:1.4_0": "ekaccaṁ vigataṁ, ekaccaṁ avigataṁ;",
  "kv1.8:1.5_0": "ekaccaṁ atthaṅgataṁ, ekaccaṁ na atthaṅgataṁ;",
  "kv1.8:1.6_0": "ekaccaṁ abbhatthaṅgataṁ, ekaccaṁ na abbhatthaṅgatanti?",
  "kv1.8:1.7_0": "Na hevaṁ vattabbe …pe….",
  "kv1.8:2.1_0": "Atītaṁ ekaccaṁ atthi, ekaccaṁ natthīti?",
  "kv1.8:2.2_0": "Āmantā.",
  "kv1.8:2.3_0": "Atītā avipakkavipākā dhammā ekacce atthi, ekacce natthīti?",
  "kv1.8:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv1.8:2.5_0": "atītaṁ ekaccaṁ atthi, ekaccaṁ natthīti?",
  "kv1.8:2.6_0": "Āmantā.",
  "kv1.8:2.7_0": "Atītā vipakkavipākā dhammā ekacce atthi, ekacce natthīti?",
  "kv1.8:2.8_0": "Na hevaṁ vattabbe …pe…",
  "kv1.8:2.9_0": "atītaṁ ekaccaṁ atthi, ekaccaṁ natthīti?",
  "kv1.8:2.10_0": "Āmantā.",
  "kv1.8:2.11_0": "Atītā avipākā dhammā ekacce atthi ekacce natthīti?",
  "kv1.8:2.12_0": "Na hevaṁ vattabbe …pe….",
  "kv1.8:3.1_0": "Atītaṁ ekaccaṁ atthi ekaccaṁ natthīti?",
  "kv1.8:3.2_0": "Āmantā.",
  "kv1.8:3.3_0": "Kiṁ atthi kiṁ natthīti?",
  "kv1.8:3.4_0": "Atītā avipakkavipākā dhammā—",
  "kv1.8:3.5_0": "te atthi;",
  "kv1.8:3.6_0": "atītā vipakkavipākā dhammā—",
  "kv1.8:3.7_0": "te natthīti.",
  "kv1.8:3.8_0": "Atītā avipakkavipākā dhammā—",
  "kv1.8:3.9_0": "te atthīti?",
  "kv1.8:3.10_0": "Āmantā.",
  "kv1.8:3.11_0": "Atītā vipakkavipākā dhammā—",
  "kv1.8:3.12_0": "te atthīti?",
  "kv1.8:3.13_0": "Na hevaṁ vattabbe …pe…",
  "kv1.8:3.14_0": "atītā avipakkavipākā dhammā—",
  "kv1.8:3.15_0": "te atthīti?",
  "kv1.8:3.16_0": "Āmantā.",
  "kv1.8:3.17_0": "Atītā avipākā dhammā—",
  "kv1.8:3.18_0": "te atthīti?",
  "kv1.8:3.19_0": "Na hevaṁ vattabbe …pe….",
  "kv1.8:4.1_0": "Atītā vipakkavipākā dhammā—",
  "kv1.8:4.2_0": "te natthīti?",
  "kv1.8:4.3_0": "Āmantā.",
  "kv1.8:4.4_0": "Atītā avipakkavipākā dhammā—",
  "kv1.8:4.5_0": "te natthīti?",
  "kv1.8:4.6_0": "Na hevaṁ vattabbe …pe….",
  "kv1.8:5.1_0": "Atītā avipākā dhammā—",
  "kv1.8:5.2_0": "te natthīti?",
  "kv1.8:5.3_0": "Āmantā.",
  "kv1.8:5.4_0": "Atītā avipakkavipākā dhammā—",
  "kv1.8:5.5_0": "te natthīti?",
  "kv1.8:5.6_0": "Na hevaṁ vattabbe …pe….",
  "kv1.8:6.1_0": "Atītā avipakkavipākā dhammā—",
  "kv1.8:6.2_0": "te atthīti?",
  "kv1.8:6.3_0": "Āmantā.",
  "kv1.8:6.4_0": "Nanu atītā avipakkavipākā dhammā niruddhāti?",
  "kv1.8:6.5_0": "Āmantā.",
  "kv1.8:6.6_0": "Hañci atītā avipakkavipākā dhammā niruddhā, no ca vata re vattabbe—",
  "kv1.8:6.7_0": "“atītā avipakkavipākā dhammā—",
  "kv1.8:6.8_0": "te atthī”ti.",
  "kv1.8:7.1_0": "Atītā avipakkavipākā dhammā niruddhā—",
  "kv1.8:7.2_0": "te atthīti?",
  "kv1.8:7.3_0": "Āmantā.",
  "kv1.8:7.4_0": "Atītā vipakkavipākā dhammā niruddhā—",
  "kv1.8:7.5_0": "te atthīti?",
  "kv1.8:7.6_0": "Na hevaṁ vattabbe …pe…",
  "kv1.8:7.7_0": "atītā avipakkavipākā dhammā niruddhā—",
  "kv1.8:7.8_0": "te atthīti?",
  "kv1.8:7.9_0": "Āmantā.",
  "kv1.8:7.10_0": "Atītā avipākā dhammā niruddhā—",
  "kv1.8:7.11_0": "te atthīti?",
  "kv1.8:7.12_0": "Na hevaṁ vattabbe …pe….",
  "kv1.8:8.1_0": "Atītā vipakkavipākā dhammā niruddhā—",
  "kv1.8:8.2_0": "te natthīti?",
  "kv1.8:8.3_0": "Āmantā.",
  "kv1.8:8.4_0": "Atītā avipakkavipākā dhammā niruddhā—",
  "kv1.8:8.5_0": "te natthīti?",
  "kv1.8:8.6_0": "Na hevaṁ vattabbe …pe….",
  "kv1.8:9.1_0": "Atītā avipākā dhammā niruddhā—",
  "kv1.8:9.2_0": "te natthīti?",
  "kv1.8:9.3_0": "Āmantā.",
  "kv1.8:9.4_0": "Atītā avipakkavipākā dhammā niruddhā—",
  "kv1.8:9.5_0": "te natthīti?",
  "kv1.8:9.6_0": "Na hevaṁ vattabbe …pe….",
  "kv1.8:10.1_0": "Atītā avipakkavipākā dhammā niruddhā—",
  "kv1.8:10.2_0": "te atthīti?",
  "kv1.8:10.3_0": "Āmantā.",
  "kv1.8:10.4_0": "Atītā vipakkavipākā dhammā niruddhā—",
  "kv1.8:10.5_0": "te natthīti?",
  "kv1.8:10.6_0": "Āmantā.",
  "kv1.8:10.7_0": "Atītā ekadesaṁ vipakkavipākā dhammā ekadesaṁ avipakkavipākā dhammā niruddhā—",
  "kv1.8:10.8_0": "te ekacce atthi ekacce natthīti?",
  "kv1.8:10.9_0": "Na hevaṁ vattabbe …pe….",
  "kv1.8:11.1_0": "Na vattabbaṁ—",
  "kv1.8:11.2_0": "“atītā avipakkavipākā dhammā—",
  "kv1.8:11.3_0": "te atthī”ti?",
  "kv1.8:11.4_0": "Āmantā.",
  "kv1.8:11.5_0": "Nanu atītā avipakkavipākā dhammā vipaccissantīti?",
  "kv1.8:11.6_0": "Āmantā.",
  "kv1.8:11.7_0": "Hañci atītā avipakkavipākā dhammā vipaccissanti, tena vata re vattabbe—",
  "kv1.8:11.8_0": "“atītā avipakkavipākā dhammā—",
  "kv1.8:11.9_0": "te atthī”ti.",
  "kv1.8:12.1_0": "Atītā avipakkavipākā dhammā vipaccissantīti katvā te atthīti?",
  "kv1.8:12.2_0": "Āmantā.",
  "kv1.8:12.3_0": "Vipaccissantīti katvā paccuppannāti?",
  "kv1.8:12.4_0": "Na hevaṁ vattabbe …pe…",
  "kv1.8:12.5_0": "vipaccissantīti katvā paccuppannāti?",
  "kv1.8:12.6_0": "Āmantā.",
  "kv1.8:12.7_0": "Paccuppannā dhammā nirujjhissantīti katvā te natthīti?",
  "kv1.8:12.8_0": "Na hevaṁ vattabbe …pe….",
  "kv1.8:13.0_0": "Anāgatādiekaccakathā",
  "kv1.8:13.1_0": "Anāgataṁ atthīti?",
  "kv1.8:13.2_0": "Ekaccaṁ atthi, ekaccaṁ natthīti.",
  "kv1.8:13.3_0": "Ekaccaṁ jātaṁ, ekaccaṁ ajātaṁ;",
  "kv1.8:13.4_0": "ekaccaṁ sañjātaṁ, ekaccaṁ asañjātaṁ;",
  "kv1.8:13.5_0": "ekaccaṁ nibbattaṁ, ekaccaṁ anibbattaṁ;",
  "kv1.8:13.6_0": "ekaccaṁ pātubhūtaṁ, ekaccaṁ apātubhūtanti?",
  "kv1.8:13.7_0": "Na hevaṁ vattabbe …pe….",
  "kv1.8:14.1_0": "Anāgataṁ ekaccaṁ atthi, ekaccaṁ natthīti?",
  "kv1.8:14.2_0": "Āmantā.",
  "kv1.8:14.3_0": "Anāgatā uppādino dhammā ekacce atthi, ekacce natthīti?",
  "kv1.8:14.4_0": "Na hevaṁ vattabbe …pe…",
  "kv1.8:14.5_0": "anāgataṁ ekaccaṁ atthi, ekaccaṁ natthīti?",
  "kv1.8:14.6_0": "Āmantā.",
  "kv1.8:14.7_0": "Anāgatā anuppādino dhammā ekacce atthi, ekacce natthīti?",
  "kv1.8:14.8_0": "Na hevaṁ vattabbe …pe….",
  "kv1.8:15.1_0": "Anāgataṁ ekaccaṁ atthi, ekaccaṁ natthīti?",
  "kv1.8:15.2_0": "Āmantā.",
  "kv1.8:15.3_0": "Kiṁ atthi, kiṁ natthīti?",
  "kv1.8:15.4_0": "Anāgatā uppādino dhammā—",
  "kv1.8:15.5_0": "te atthi;",
  "kv1.8:15.6_0": "anāgatā anuppādino dhammā—",
  "kv1.8:15.7_0": "te natthīti.",
  "kv1.8:15.8_0": "Anāgatā uppādino dhammā—",
  "kv1.8:15.9_0": "te atthīti?",
  "kv1.8:15.10_0": "Āmantā.",
  "kv1.8:15.11_0": "Anāgatā anuppādino dhammā—",
  "kv1.8:15.12_0": "te atthīti?",
  "kv1.8:15.13_0": "Na hevaṁ vattabbe …pe…",
  "kv1.8:15.14_0": "anāgatā anuppādino dhammā—",
  "kv1.8:15.15_0": "te natthīti?",
  "kv1.8:15.16_0": "Āmantā.",
  "kv1.8:15.17_0": "Anāgatā uppādino dhammā—",
  "kv1.8:15.18_0": "te natthīti?",
  "kv1.8:15.19_0": "Na hevaṁ vattabbe …pe….",
  "kv1.8:16.1_0": "Anāgatā uppādino dhammā—",
  "kv1.8:16.2_0": "te atthīti?",
  "kv1.8:16.3_0": "Āmantā.",
  "kv1.8:16.4_0": "Nanu anāgatā uppādino dhammā ajātāti?",
  "kv1.8:16.5_0": "Āmantā.",
  "kv1.8:16.6_0": "Hañci anāgatā uppādino dhammā ajātā, no ca vata re vattabbe—",
  "kv1.8:16.7_0": "“anāgatā uppādino dhammā—",
  "kv1.8:16.8_0": "te atthī”ti.",
  "kv1.8:17.1_0": "Anāgatā uppādino dhammā ajātā—",
  "kv1.8:17.2_0": "te atthīti?",
  "kv1.8:17.3_0": "Āmantā.",
  "kv1.8:17.4_0": "Anāgatā anuppādino dhammā ajātā—",
  "kv1.8:17.5_0": "te atthīti?",
  "kv1.8:17.6_0": "Na hevaṁ vattabbe …pe…",
  "kv1.8:17.7_0": "anāgatā anuppādino dhammā ajātā—",
  "kv1.8:17.8_0": "te natthīti?",
  "kv1.8:17.9_0": "Āmantā.",
  "kv1.8:17.10_0": "Anāgatā uppādino dhammā ajātā—",
  "kv1.8:17.11_0": "te natthīti?",
  "kv1.8:17.12_0": "Na hevaṁ vattabbe …pe….",
  "kv1.8:18.1_0": "Na vattabbaṁ—",
  "kv1.8:18.2_0": "“anāgatā uppādino dhammā—",
  "kv1.8:18.3_0": "te atthī”ti?",
  "kv1.8:18.4_0": "Āmantā.",
  "kv1.8:18.5_0": "Nanu anāgatā uppādino dhammā uppajjissantīti?",
  "kv1.8:18.6_0": "Āmantā.",
  "kv1.8:18.7_0": "Hañci anāgatā uppādino dhammā uppajjissanti, tena vata re vattabbe—",
  "kv1.8:18.8_0": "“anāgatā uppādino dhammā—",
  "kv1.8:18.9_0": "te atthī”ti.",
  "kv1.8:19.1_0": "Anāgatā uppādino dhammā uppajjissantīti katvā te atthīti?",
  "kv1.8:19.2_0": "Āmantā.",
  "kv1.8:19.3_0": "Uppajjissantīti katvā paccuppannāti?",
  "kv1.8:19.4_0": "Na hevaṁ vattabbe …pe…",
  "kv1.8:19.5_0": "uppajjissantīti katvā paccuppannāti?",
  "kv1.8:19.6_0": "Āmantā.",
  "kv1.8:19.7_0": "Paccuppannā dhammā nirujjhissantīti katvā te natthīti?",
  "kv1.8:19.8_0": "Na hevaṁ vattabbe …pe….",
  "kv1.8:19.9_0": "Ekaccaṁ atthītikathā niṭṭhitā."
}