{
  "anya-e0607n:0_0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e0607n:1_0": "Pajjamadhu",
  "anya-e0607n:2_0": "Ānanda rañña ratanādi mahā yatinda,",
  "anya-e0607n:3_0": "Niccappabuddha padumappiya sevinaṅgī;",
  "anya-e0607n:4_0": "Buddhappiyena ghana buddha guṇappiyena,",
  "anya-e0607n:5_0": "Therālinā racita pajjamadhuṁ pibantu.",
  "anya-e0607n:6_0": "1.",
  "anya-e0607n:7_0": "Uṇṇāpapuṇṇasasimaṇdalato galitvā,",
  "anya-e0607n:8_0": "Pādambujaṅguli dalaṭṭha sudhā lavānaṁ;",
  "anya-e0607n:9_0": "Pantīva satthu nakhapanti pajāvisesaṁ,",
  "anya-e0607n:10_0": "Pīṇetu suddha sukhitammaṇa tunḍapītā.",
  "anya-e0607n:11_0": "2.",
  "anya-e0607n:12_0": "Khittāya māraripunā parivatya satthu,",
  "anya-e0607n:13_0": "Pādassayā jita disāya sitattalāya;",
  "anya-e0607n:14_0": "Yā jeti kañcana sarāvaliyā siriṁ sā,",
  "anya-e0607n:15_0": "Detanginaṁ raṇajayaṅgulipantikantā.",
  "anya-e0607n:16_0": "3.",
  "anya-e0607n:17_0": "Sovaṇṇa vaṇṇa sukhumacchavi somma kumma,",
  "anya-e0607n:18_0": "Piṭṭhīva piṭṭhi kamatunnati bhāti yesaṁ;",
  "anya-e0607n:19_0": "Tesuppatiṭṭhitasukomaladīghapaṇhi,",
  "anya-e0607n:20_0": "Pādā jinassa padadantu padaṁ janassa.",
  "anya-e0607n:21_0": "4.",
  "anya-e0607n:22_0": "Acchera paṅkajasiraṁ siriyā sakāya,",
  "anya-e0607n:23_0": "Ye maddino viya caranti saroja sīse;",
  "anya-e0607n:24_0": "Sañcumbitā viya ca tāni parāga rāgā,",
  "anya-e0607n:25_0": "Te nīrajā munipadā padadantu lakkhiṁ.",
  "anya-e0607n:26_0": "5.",
  "anya-e0607n:27_0": "Agāmi kāla jana maṅgala bhattu bhāvaṁ,",
  "anya-e0607n:28_0": "Vyākattumatra kusaleniva nimmitāni;",
  "anya-e0607n:29_0": "Yātrāsumaṭṭhasatamaṅgalakkhaṇāni,",
  "anya-e0607n:30_0": "Sādhetu naṁ padayugaṁ jayamaṅgalāni.",
  "anya-e0607n:31_0": "6.",
  "anya-e0607n:32_0": "Sassevijantuvarasantipurappavese,",
  "anya-e0607n:33_0": "Niccaṁ susajja ṭhapitāniva maṅgalāya;",
  "anya-e0607n:34_0": "Ye te dadhanti kalamaṅgalalakkhaṇāni,",
  "anya-e0607n:35_0": "Vattantu te jinapadā jayamaṅgalāya.",
  "anya-e0607n:36_0": "7.",
  "anya-e0607n:37_0": "Sabbebhibhūya sapadesu nipātanassa,",
  "anya-e0607n:38_0": "Saññāṇakaṁ viya yadassitasabbaloko;",
  "anya-e0607n:39_0": "Pādātyadhokatatilokasirovarā pi,",
  "anya-e0607n:40_0": "Lokaṁ puṇantu jayamaṅgalakāraṇāni.",
  "anya-e0607n:41_0": "8.",
  "anya-e0607n:42_0": "Lokattayekasaraṇattavibhāvanāya,",
  "anya-e0607n:43_0": "Sajjo va tiṭṭhati yahiṁ suvibhattaloko;",
  "anya-e0607n:44_0": "Taṁ sabbalokapaṭibimbitadappaṇābhaṁ,",
  "anya-e0607n:45_0": "Pādadvayaṁ janasusajjanahetu hotu.",
  "anya-e0607n:46_0": "9.",
  "anya-e0607n:47_0": "Lokuttarāya siriyādhigamāya suṭṭhu,",
  "anya-e0607n:48_0": "Rajanti yattha diguṇāniva pātu bhūtā;",
  "anya-e0607n:49_0": "Cakkāsanābhisahanemisahassarāni,",
  "anya-e0607n:50_0": "Tyaṅghī disantu sakalissariyaṁ janassa.",
  "anya-e0607n:51_0": "10.",
  "anya-e0607n:52_0": "Yatrullasanti duvidhāniva pātū bhūtā,",
  "anya-e0607n:53_0": "Dhammassasabbabhuvanassa ca issaratte;",
  "anya-e0607n:54_0": "Cakkāni cakkasadisāni sudassanassa,",
  "anya-e0607n:55_0": "Tānajja jantu saraṇā caraṇāni hontu.",
  "anya-e0607n:56_0": "11.",
  "anya-e0607n:57_0": "Sattesu vacchatu sirī sirivacchakena,",
  "anya-e0607n:58_0": "Sovatthi sotthimanutiṭṭhatu puggalesu;",
  "anya-e0607n:59_0": "Nandiṁ janānamanuvattatu nandivattī,",
  "anya-e0607n:60_0": "Sīsānalaṁkurutu pādavataṁsako pi.",
  "anya-e0607n:61_0": "12.",
  "anya-e0607n:62_0": "Bhaddāya pīṭhamupagacchatu bhaddapīṭhaṁ,",
  "anya-e0607n:63_0": "Vuddhiṁ janānamanuvattatu vaddhamānaṁ;",
  "anya-e0607n:64_0": "Puṇṇattamaṅgimanukubbatu puṇṇakumbho,",
  "anya-e0607n:65_0": "Pāti ca pātu satataṁ janataṁ apāyā.",
  "anya-e0607n:66_0": "13.",
  "anya-e0607n:67_0": "Setātapattamapanetamaghātape taṁ,",
  "anya-e0607n:68_0": "Khaggo vichindatu sadā duritārivagge;",
  "anya-e0607n:69_0": "Saṁklesadāhamapanetu satālavanṭa,",
  "anya-e0607n:70_0": "Saṁvījanī kumatimakkhikamorahattho.",
  "anya-e0607n:71_0": "14.",
  "anya-e0607n:72_0": "Ākaḍḍhano janavilocanamattaninnaṁ,",
  "anya-e0607n:73_0": "Vāretu sabbagativāranamaṅkuso so;",
  "anya-e0607n:74_0": "Pādambujassirivilāsaniketanaṁ va,",
  "anya-e0607n:75_0": "Pāsādalakhaṇamupetu manopasādaṁ.",
  "anya-e0607n:76_0": "15.",
  "anya-e0607n:77_0": "Pāṇīnamattabhajataṁ varapuṇṇapattaṁ,",
  "anya-e0607n:78_0": "Sammā dadātu padanissitapuṇṇapatto;",
  "anya-e0607n:79_0": "Pādesu jantu manabandhanadāmabhūtaṁ,",
  "anya-e0607n:80_0": "Dāmaṁ dametu vimalaṁ janatammanāni.",
  "anya-e0607n:81_0": "16.",
  "anya-e0607n:82_0": "Uṇhīsakuppalamaṇīpadumehi pādā,",
  "anya-e0607n:83_0": "Sassevijantukaraṇāni vibhūsayantu;",
  "anya-e0607n:84_0": "Sannettanāvupagatānamanagghakāni,",
  "anya-e0607n:85_0": "Bojjhaṅgasattaratanāni dade samuddo.",
  "anya-e0607n:86_0": "17.",
  "anya-e0607n:87_0": "Uttuṅga niccalaguṅā jitatāya niccaṁ,",
  "anya-e0607n:88_0": "Sevīva pādasiri nicca samubbahaṁ va;",
  "anya-e0607n:89_0": "Atrāpi sakkabhavanubbahaṇe niyutto,",
  "anya-e0607n:90_0": "Pādaṭṭhameru bhavataṁ bhavataṁ vibhūtyā.",
  "anya-e0607n:91_0": "18.",
  "anya-e0607n:92_0": "So cakkavāḷasikharī pyavataṁ samantā,",
  "anya-e0607n:93_0": "Sabbūpasaggavisarājanataṁ samaggaṁ;",
  "anya-e0607n:94_0": "Dīpā puthūpi caturo dvisahassa khuddā,",
  "anya-e0607n:95_0": "Dhārentvapāyapatamānamadatva jantuṁ.",
  "anya-e0607n:96_0": "19.",
  "anya-e0607n:97_0": "Sūro pabodhayatu jantu saroruhāni,",
  "anya-e0607n:98_0": "Cando pasāda kumudāni manodahesu;",
  "anya-e0607n:99_0": "Nakkhattajātamakhilaṁ subhatāya hotu,",
  "anya-e0607n:100_0": "Cakkaṁ dhajaṁ ripujayāya jayaddhajāya.",
  "anya-e0607n:101_0": "20.",
  "anya-e0607n:102_0": "Jetuṁ sasaṁsada-sudassana-cakkavatti,",
  "anya-e0607n:103_0": "Cakkānugantalalitaṁ yahimāvaheyya;",
  "anya-e0607n:104_0": "Cakkāṇuvatti parisāvata-cakkavatti,",
  "anya-e0607n:105_0": "Naṁvattataṁ padayugaṁ janatā hitāya.",
  "anya-e0607n:106_0": "21.",
  "anya-e0607n:107_0": "Pujetumāgata vatā vajirāsanaṭṭha,",
  "anya-e0607n:108_0": "Mindena chaḍḍita mahāvijayuttarākhyaṁ;",
  "anya-e0607n:109_0": "Saṁkhaṁ paviṭṭhamiva mārabhayā padādho,",
  "anya-e0607n:110_0": "Pādaṭṭhasaṁkhamiha vattatu santiyā vo.",
  "anya-e0607n:111_0": "22.",
  "anya-e0607n:112_0": "Sovaṇṇamacchayugalaṁ sivabhatta bhoge,",
  "anya-e0607n:113_0": "Icchā bahūpakaraṇaṁ bhavataṁ janānaṁ;",
  "anya-e0607n:114_0": "Kumbhīladhiggahitato va padutthacittā,",
  "anya-e0607n:115_0": "Pādambujākara vigāhi tu nopahontu.",
  "anya-e0607n:116_0": "23.",
  "anya-e0607n:117_0": "Sattāpagā janamanoja male jahantu,",
  "anya-e0607n:118_0": "Saṁklesadāhamapanentu dahā ca satta;",
  "anya-e0607n:119_0": "Selā ca satta vidadhantu janassa tānaṁ,",
  "anya-e0607n:120_0": "Lokappasiddhijanane bhavataṁ patākā.",
  "anya-e0607n:121_0": "24.",
  "anya-e0607n:122_0": "Pāṭaṅki santi gamane bhavatūpakārā,",
  "anya-e0607n:123_0": "Dāhettanesu jahataṁ padacāmaraṁ taṁ;",
  "anya-e0607n:124_0": "Sallokalocanamahussava-ussitaṁ va,",
  "anya-e0607n:125_0": "Vatteyya toraṇamanuttaramaṅgalāya.",
  "anya-e0607n:126_0": "25.",
  "anya-e0607n:127_0": "Yasmiṁ miginda gata bhīti balāva daḍḍha,",
  "anya-e0607n:128_0": "Dānā natā siravidāraṇa pīḷitāva;",
  "anya-e0607n:129_0": "Nālāgirī karivaro girimekhalo ca,",
  "anya-e0607n:130_0": "Taṁ sīhavikkamapadaṁ hanatā ghadantiṁ.",
  "anya-e0607n:131_0": "26.",
  "anya-e0607n:132_0": "Pāpāhino hanatu pādasuvaṇṇarājā,",
  "anya-e0607n:133_0": "Vyagghādhipo kalijane adataṁ asesaṁ;",
  "anya-e0607n:134_0": "Vālāha-assapati sampatituṁ adatvā,",
  "anya-e0607n:135_0": "Pāyesu pāpayatu santipurampajāyo.",
  "anya-e0607n:136_0": "27.",
  "anya-e0607n:137_0": "Chaddanta danti lalitaṁ galitaṁ rusamhā,",
  "anya-e0607n:138_0": "Luddetta dubbhini dise acalaṁ dadhāno;",
  "anya-e0607n:139_0": "Pādaṭṭhahatthipati sampati jantutāse,",
  "anya-e0607n:140_0": "Tāsetu hāsamaparandisataṁ satānaṁ.",
  "anya-e0607n:141_0": "28.",
  "anya-e0607n:142_0": "Sabbaṅgino caraṇuposatha hatthirājā,",
  "anya-e0607n:143_0": "Pāpetu sabbacatudīpikarajjalakkhiṁ;",
  "anya-e0607n:144_0": "Kittīva pādaparicārikatā niyuttā,",
  "anya-e0607n:145_0": "Kelasaselapaṭimā hitamācareyya.",
  "anya-e0607n:146_0": "29.",
  "anya-e0607n:147_0": "Sāmissa haṁsasamaye dahapāsabaddha,",
  "anya-e0607n:148_0": "Māsīna vesagamako viya pādahaṁso;",
  "anya-e0607n:149_0": "Nigghosa gantijitato viya mūgapakkho,",
  "anya-e0607n:150_0": "Yāretu sabba janatā bhavagantukattaṁ.",
  "anya-e0607n:151_0": "30.",
  "anya-e0607n:152_0": "Ohāya dibbasarasiṁ khilaloka sabba,",
  "anya-e0607n:153_0": "Rammaṅghivāpimavagāhitavāva pāde;",
  "anya-e0607n:154_0": "Erāvaṇo karivaro manasābhiruḷhe,",
  "anya-e0607n:155_0": "Jantuṁ purindadapuraṁ nayataṁ va sīghaṁ.",
  "anya-e0607n:156_0": "31.",
  "anya-e0607n:157_0": "Hitvā sakambhavanamaṅghinisevanattha,",
  "anya-e0607n:158_0": "Māgamma ramma taratāyiha nissito va;",
  "anya-e0607n:159_0": "Pāletva mūni padavāpitaraṅgabhaṅgi,",
  "anya-e0607n:160_0": "Mangī karontatanuvāsuki nāgarājā.",
  "anya-e0607n:161_0": "32.",
  "anya-e0607n:162_0": "Nāthassa kañcanasikhāvalajātilīla,",
  "anya-e0607n:163_0": "Māvikaraṁ va padanissitamorarājā;",
  "anya-e0607n:164_0": "Taṁ dhammadesanaraveniva luddakassa,",
  "anya-e0607n:165_0": "Lokassa pāpaphaṇino hanataṁ asesaṁ.",
  "anya-e0607n:166_0": "33.",
  "anya-e0607n:167_0": "Saṁsārasāgaragate sadhane jane te,",
  "anya-e0607n:168_0": "Netampade kalacatummukhahemanāvā;",
  "anya-e0607n:169_0": "Nibbāṇapaṭṭanavaraṁ bharukacchakantaṁ,",
  "anya-e0607n:170_0": "Suppārapaṇḍita gatā viya āsunāvā.",
  "anya-e0607n:171_0": "34.",
  "anya-e0607n:172_0": "Sambodhi ñāṇa paripācayato munissa,",
  "anya-e0607n:173_0": "Bhatto yathā himavataddi samādhihetu;",
  "anya-e0607n:174_0": "Evammanena bhajataṁ himavaddipāde,",
  "anya-e0607n:175_0": "Sambodhiñāṇa paripācanahetu hotu.",
  "anya-e0607n:176_0": "35.",
  "anya-e0607n:177_0": "Daḷhaṁ parājitatayā muninā sarena,",
  "anya-e0607n:178_0": "Suññassaropagata pañjara bandhanova;",
  "anya-e0607n:179_0": "So pādapañjaragato karavīkapakkhī,",
  "anya-e0607n:180_0": "Sabbesamappīyāvacañjahatā bhavantaṁ.",
  "anya-e0607n:181_0": "36.",
  "anya-e0607n:182_0": "Te cakkavāka makarā api koñca jīvaṁ,",
  "anya-e0607n:183_0": "Jīvādi pakkhivisarā sarasīva bhuttaṁ;",
  "anya-e0607n:184_0": "Vessantarena caraṇambuji nibbhajantā,",
  "anya-e0607n:185_0": "Jantu tahiṁ viya pade suramentu niccaṁ.",
  "anya-e0607n:186_0": "37.",
  "anya-e0607n:187_0": "Taṁ candakinnaragatiṁva gatassa bodhi,",
  "anya-e0607n:188_0": "Sattassa tassa sapajāpatikassa bhāvaṁ;",
  "anya-e0607n:189_0": "Saṁsūcayanta pada kinnara kinnarī ve,",
  "anya-e0607n:190_0": "Sāmaggimagga paṭi pattisu pāpayantu.",
  "anya-e0607n:191_0": "38.",
  "anya-e0607n:192_0": "Saṁrājadhānimusabho vahatagga bhāraṁ,",
  "anya-e0607n:193_0": "Pītippayo pajanayeyya savacchadhenu;",
  "anya-e0607n:194_0": "Sassevino abhiramentu chakāmasaggā,",
  "anya-e0607n:195_0": "Dhārentu jhāyimiha soḷasa dhātudhāmā.",
  "anya-e0607n:196_0": "39.",
  "anya-e0607n:197_0": "Sutvā jinassa karavīka sarammanuññaṁ,",
  "anya-e0607n:198_0": "Aññoñña bhītirahitā api paccanīkā;",
  "anya-e0607n:199_0": "Hitvā gatiṁ viya ṭhitā padasattarūpā,",
  "anya-e0607n:200_0": "Sabbaṁ bhavassita janānagatiṁ hanantu.",
  "anya-e0607n:201_0": "40.",
  "anya-e0607n:202_0": "Sovaṇṇa kāhaḷa yugo pamamindirāya,",
  "anya-e0607n:203_0": "Sannīrapuppha mukulopamamussavāya;",
  "anya-e0607n:204_0": "Niccaṁ susajja ṭhapitaṁ muni tiṭṭhatante,",
  "anya-e0607n:205_0": "Jaṅghādvayaṁ janavilocana maṅgalāya.",
  "anya-e0607n:206_0": "41.",
  "anya-e0607n:207_0": "Lakhyā vilāsa mukuradvaya sannikāsaṁ,",
  "anya-e0607n:208_0": "Tāḍaṅka maṇḍana viḍambakamaṁsu saṇḍaṁ;",
  "anya-e0607n:209_0": "Jānudvayaṁ laḷita sāgara bubbalābhaṁ,",
  "anya-e0607n:210_0": "Hotaṁ jagattaya nijatta vibhūsitunte.",
  "anya-e0607n:211_0": "42.",
  "anya-e0607n:212_0": "Chaddanti dinna varadanta yugopamānā,",
  "anya-e0607n:213_0": "Taṁ hatthi soṇḍa kama puṇṇa guṇā tavorū;",
  "anya-e0607n:214_0": "Līla payodhi siri keḷi suvaṇṇarambhā,",
  "anya-e0607n:215_0": "Khandhāva dentu paripuṇṇa gune janānāṁ.",
  "anya-e0607n:216_0": "43.",
  "anya-e0607n:217_0": "Jaṅghakkha kadvya samappita cittapāda,",
  "anya-e0607n:218_0": "Cakkadvayī manamanojahayo mune te;",
  "anya-e0607n:219_0": "Sonī ratho sirivaho manasā bhiruḷhaṁ,",
  "anya-e0607n:220_0": "Lokattayaṁ sivapuraṁ lahu pāpayātu.",
  "anya-e0607n:221_0": "44.",
  "anya-e0607n:222_0": "Rammora pākaṭa taṭāka taṭā savanta,",
  "anya-e0607n:223_0": "Romāvalī jala panālika koṭikaṭṭhā;",
  "anya-e0607n:224_0": "Nābhī gabhīra sarasī siri keḷitā te,",
  "anya-e0607n:225_0": "Sassevinaṁ vyasana ghammamalaṁ sametu.",
  "anya-e0607n:226_0": "45.",
  "anya-e0607n:227_0": "Kanticchaṭā luḷita rūpa payodhi nābhi,",
  "anya-e0607n:228_0": "Āvaṭṭa vaṭṭita nimujjita sabbaloko;",
  "anya-e0607n:229_0": "Sobhagga toya nivahaṁ visaso pivitvā,",
  "anya-e0607n:230_0": "Lokuttarādi sukha mucchitataṁ payātu.",
  "anya-e0607n:231_0": "46.",
  "anya-e0607n:232_0": "Gambhīra cittarahadaṁ paripūrayitvā,",
  "anya-e0607n:233_0": "Taṁ sandamāna karuṇambu pavāha tulyā;",
  "anya-e0607n:234_0": "Romālivallihari nābhi subhālavālā,",
  "anya-e0607n:235_0": "Detaṁ lahuṁ sivaphalaṁ bhajataṁ mune te.",
  "anya-e0607n:236_0": "47.",
  "anya-e0607n:237_0": "Cārūra sāriphalako kuṭilagga loma,",
  "anya-e0607n:238_0": "Pantī vibhatti sahito siri keḷi sajjo;",
  "anya-e0607n:239_0": "Saggāpavagga sukha jūtaka keḷi hetu,",
  "anya-e0607n:240_0": "Hotaṁ tiloka sukha jūtaka soṇḍakānaṁ.",
  "anya-e0607n:241_0": "48.",
  "anya-e0607n:242_0": "Gambhīra citta rahado dara gāhamāna,",
  "anya-e0607n:243_0": "Mettādayā kari vadhū kara sanni kāsā;",
  "anya-e0607n:244_0": "Sabbaṅginaṁ sivaphalaṁ tanu deva rukkhe,",
  "anya-e0607n:245_0": "Sākhā sakhā tava bhujā bhajataṁ dadantu.",
  "anya-e0607n:246_0": "49.",
  "anya-e0607n:247_0": "Nihāra bindu sahitaggadalopa sobhi,",
  "anya-e0607n:248_0": "Byālamba ratta padumadvaya bhaṅgi bhājā;",
  "anya-e0607n:249_0": "Pāpārisīsalunateniva ratta rattā,",
  "anya-e0607n:250_0": "Rattā karā tava bhavumbhuvi maṅgalāya.",
  "anya-e0607n:251_0": "50.",
  "anya-e0607n:252_0": "Rupassirī carita caṅkama vibbhamā te,",
  "anya-e0607n:253_0": "Piṭṭhī yathā kalala muddhani setu bhūtā;",
  "anya-e0607n:254_0": "Evaṁ bhavaṇṇava samuttaraṇāya setu,",
  "anya-e0607n:255_0": "Hotammahākanaka saṁkama sannikāsā.",
  "anya-e0607n:256_0": "51.",
  "anya-e0607n:257_0": "Saddhamma desana manohara bherināda,",
  "anya-e0607n:258_0": "Saṁcāraṇe sivapuraṁ visituṁ janānaṁ;",
  "anya-e0607n:259_0": "Gīvā suvaṇṇamaya cāru mutiṅga bheri,",
  "anya-e0607n:260_0": "Bhāvambhajā bhavatu bhūta vibhūtiyā te.",
  "anya-e0607n:261_0": "52.",
  "anya-e0607n:262_0": "Lakhī nivāsa vadanambuja matta ninna,",
  "anya-e0607n:263_0": "Mākaḍḍhayaṁ jana vilocana cañcarīke;",
  "anya-e0607n:264_0": "Sorabbha dhamma makaranda nisandamānaṁ,",
  "anya-e0607n:265_0": "Piṇetu tena sarasena sabhā jane te.",
  "anya-e0607n:266_0": "53.",
  "anya-e0607n:267_0": "Lakhī samāruhita vattarathe rathaṅga,",
  "anya-e0607n:268_0": "Dvandānu kāri miga rāja kapola līlaṁ;",
  "anya-e0607n:269_0": "Tādaṅka maṇḍalayugaṁ viya kaṇṇabhājaṁ,",
  "anya-e0607n:270_0": "Gaṇḍatthaladvyamalaṁkurutaṁ janatte.",
  "anya-e0607n:271_0": "54.",
  "anya-e0607n:272_0": "Lāvaṇṇa maṇṇava pavāḷa latā dvayābhaṁ,",
  "anya-e0607n:273_0": "Tandeha deva taru pallava kante mantaṁ;",
  "anya-e0607n:274_0": "Vattāravinda makaranda parājisobhaṁ,",
  "anya-e0607n:275_0": "Rattādharadvayamadho kurutaṁ janāghaṁ.",
  "anya-e0607n:276_0": "55.",
  "anya-e0607n:277_0": "Uṇṇā sakuntigata matthaka natthu kūpa,",
  "anya-e0607n:278_0": "Subbhū lakāra sahitoṭṭha pavāḷa nāvā;",
  "anya-e0607n:279_0": "Gattuttararaṇṇava gatā tava jantukānaṁ,",
  "anya-e0607n:280_0": "Hotaṁ bhavaṇṇava samuttaranaya nātha.",
  "anya-e0607n:281_0": "56.",
  "anya-e0607n:282_0": "Isaṁ vikāsa padumodara kesarāli,",
  "anya-e0607n:283_0": "Līlā vinaddha rucirā tava danta panti;",
  "anya-e0607n:284_0": "Vānī vadhū dharita mālati mālya tulyā,",
  "anya-e0607n:285_0": "Tassaṁ jānassa manarañjana mācareyya.",
  "anya-e0607n:286_0": "57.",
  "anya-e0607n:287_0": "Saddhamma nijjhara suratta silātalābhā,",
  "anya-e0607n:288_0": "Jivhā vacī naṭa vadhū kala raṅga bhūtā;",
  "anya-e0607n:289_0": "Saddhamma seṭṭha taraṇī nihitappiyā te,",
  "anya-e0607n:290_0": "Saṁsāra sāgara samuttaraṇāya hotu.",
  "anya-e0607n:291_0": "58.",
  "anya-e0607n:292_0": "Dantaṁsu kañcukīta rattadharo padhāne,",
  "anya-e0607n:293_0": "Jivhā suratta sayane mukha mandiraṭṭhe;",
  "anya-e0607n:294_0": "Āmokkha mutti vadhuyā sayitāya tuyhaṁ,",
  "anya-e0607n:295_0": "Kubbantu saṁgama malaṁ jana sotu kāmi.",
  "anya-e0607n:296_0": "59.",
  "anya-e0607n:297_0": "Uṇṇā tathābhinava patta varābhi rāmā,",
  "anya-e0607n:298_0": "Līlollasanta bhamukadvaya nīla pattā;",
  "anya-e0607n:299_0": "Ghānoru cāru kadalī vadanā lavālā,",
  "anya-e0607n:300_0": "Tuyhaṁ pavattatu ciraṁ jana maṅgalāya.",
  "anya-e0607n:301_0": "60.",
  "anya-e0607n:302_0": "Bālatthalī hari silātala piṭṭhikaṭṭha,",
  "anya-e0607n:303_0": "Bhūvallaridvaya mayūra yugassa tuyhaṁ;",
  "anya-e0607n:304_0": "Pañcappabhā rucira piccha yugassirīkaṁ,",
  "anya-e0607n:305_0": "Nettadvayaṁ manasi puñchatu pāpadhūliṁ.",
  "anya-e0607n:306_0": "61.",
  "anya-e0607n:307_0": "Indīvarāntagata bhiṅgika panti bhaṅgi,",
  "anya-e0607n:308_0": "Pañambujassaratate viya gacchapantī;",
  "anya-e0607n:309_0": "Nettambujassiri tirokaraṇīva tuyhaṁ,",
  "anya-e0607n:310_0": "Pamhāvalī sirigateha tiro karontu.",
  "anya-e0607n:311_0": "62.",
  "anya-e0607n:312_0": "Vattullasambuja vilocana haṁsa tuṇḍa,",
  "anya-e0607n:313_0": "Kañjaṁsu piñjara mulāla latā dvyābhaṁ;",
  "anya-e0607n:314_0": "Dolādvayaṁva savaṇadvyamatta lakkhyā,",
  "anya-e0607n:315_0": "Hotaṁ tavajja janatā maticārahetu.",
  "anya-e0607n:316_0": "63.",
  "anya-e0607n:317_0": "Vammīka matthaka sayānaka bhūridatta,",
  "anya-e0607n:318_0": "Bhoginda bhogavali vibbhamamā vahanti;",
  "anya-e0607n:319_0": "Ghānopariṭṭhitamune tava tuṇṇamuṇṇā,",
  "anya-e0607n:320_0": "Taggāhino viya janassa dadātu vittaṁ.",
  "anya-e0607n:321_0": "64.",
  "anya-e0607n:322_0": "Rūpindirāya vijaye khila loka rūpaṁ,",
  "anya-e0607n:323_0": "Ghāṇoru cāru parighopari baddha siddhā;",
  "anya-e0607n:324_0": "Nīlābha vāta viluthanta vayaddhajābhā,",
  "anya-e0607n:325_0": "Tiṭṭhantu sajja duritāri jayāya te bhū.",
  "anya-e0607n:326_0": "65.",
  "anya-e0607n:327_0": "Uṇṇassitopala nivesita bunda sandhi,",
  "anya-e0607n:328_0": "Ghāṇoru piṇḍakamaghā tapa rundhitunte;",
  "anya-e0607n:329_0": "Hotammukhambuja sirī sirasussitābhaṁ,",
  "anya-e0607n:330_0": "Bhū nīla paṭṭika lalāta suvaṇṇa chattaṁ.",
  "anya-e0607n:331_0": "66.",
  "anya-e0607n:332_0": "Rupaṅka vedana vilocana bāna diṭṭhī,",
  "anya-e0607n:333_0": "Dhārā nisāna maṇivaṭṭa sirī siro te;",
  "anya-e0607n:334_0": "Siddhā matosadha katañjana puñja lakkhī,",
  "anya-e0607n:335_0": "Hotaṁ janassa nayanāmaya nāsanāya.",
  "anya-e0607n:336_0": "67.",
  "anya-e0607n:337_0": "Sakkhandha bāhuyuga torana majjha gīvā,",
  "anya-e0607n:338_0": "Dharappitassirighato pari mussavāya;",
  "anya-e0607n:339_0": "Nīluppalāva ṭhapitā savibhatti kante,",
  "anya-e0607n:340_0": "Kesā bhavantu bhuvanattaya maṅgalāya.",
  "anya-e0607n:341_0": "68.",
  "anya-e0607n:342_0": "Hemagghiye ṭhapita nīla silā kapāle,",
  "anya-e0607n:343_0": "Pajjota jāla lalitaṁ muni sārayantī;",
  "anya-e0607n:344_0": "Rūpassirī sirasi bhūsita hema mālā,",
  "anya-e0607n:345_0": "Kārā karotu subhagaṁ tava ketu mālā.",
  "anya-e0607n:346_0": "69.",
  "anya-e0607n:347_0": "Bhyāmappabhāli tava kañcana mora kāle,",
  "anya-e0607n:348_0": "Surodaye vitata candaka cakkalakkhī;",
  "anya-e0607n:349_0": "Meghāvanaddha sikharunnata hema selā,",
  "anya-e0607n:350_0": "Yantindacāpa vikatīva dadātu sobhaṁ.",
  "anya-e0607n:351_0": "70.",
  "anya-e0607n:352_0": "Paṭṭhāya te paṇidhito suci dāna sīla,",
  "anya-e0607n:353_0": "Nekkhamma pañña viriyakkhama saccadhiṭṭhā;",
  "anya-e0607n:354_0": "Mettā upekkhiti ime dasa pūratova,",
  "anya-e0607n:355_0": "Pūrentu pārami guṇā janatānamatte.",
  "anya-e0607n:356_0": "71.",
  "anya-e0607n:357_0": "Pattuttaruttaradasā paṇidhāna bījā,",
  "anya-e0607n:358_0": "Cetoradharāya karuṇā jala sekha vuddhā;",
  "anya-e0607n:359_0": "Sabbaññu ñāṇa phaladā sati vāṭa guttā,",
  "anya-e0607n:360_0": "Taṁ samphalandisatu pāramitā latā te.",
  "anya-e0607n:361_0": "72.",
  "anya-e0607n:362_0": "Ābodhi puṇṇami padiṭṭha dinādito te,",
  "anya-e0607n:363_0": "Sambhāra kāla sita pakkha kamābhi vuddho;",
  "anya-e0607n:364_0": "Sampuṇṇa pārami guṇāmataraṁsi taṁva,",
  "anya-e0607n:365_0": "Sabbaṅgi kunda kumudāni pabodhayeyya.",
  "anya-e0607n:366_0": "73.",
  "anya-e0607n:367_0": "Āpacchimabbhava sivapphala lābha dānā,",
  "anya-e0607n:368_0": "Dānappabandhamapidāna phalappabhandaṁ;",
  "anya-e0607n:369_0": "Saṁvaḍḍhayi tvaṁ abhipatthanato yathevaṁ,",
  "anya-e0607n:370_0": "Jantuttaruttara phalaṁ khalu sambhunantu.",
  "anya-e0607n:371_0": "74.",
  "anya-e0607n:372_0": "Ārambhatoppabhuti yāva tavaggamaggā,",
  "anya-e0607n:373_0": "Vikkhālita ghakalusaṁ suci sīla toyaṁ;",
  "anya-e0607n:374_0": "Mettā dayā madhura sītalatāyupetaṁ,",
  "anya-e0607n:375_0": "Sodhetu tvaṁva bhava nissita jantu metaṁ.",
  "anya-e0607n:376_0": "75.",
  "anya-e0607n:377_0": "Āpaccimattamabhinikkhamanābhiyogā,",
  "anya-e0607n:378_0": "Paṭṭhāya tampabhavato paripuṇṇa gehā;",
  "anya-e0607n:379_0": "Tvaṁ sabba jāti gahato api nikkhamittho,",
  "anya-e0607n:380_0": "Evaṁ janā bhava dukhā khalu nikkhamantu.",
  "anya-e0607n:381_0": "76.",
  "anya-e0607n:382_0": "Ekaggato pala tale nisitā cirandhi,",
  "anya-e0607n:383_0": "Dhārā sucittu sutale sati daṇḍa baddhe;",
  "anya-e0607n:384_0": "Nibbijjhi lakkhaṇa dhanuṭṭhiti santi lakkhaṁ,",
  "anya-e0607n:385_0": "Khittā tayonamanu vijjhatu jantu khittā.",
  "anya-e0607n:386_0": "77.",
  "anya-e0607n:387_0": "Tvaṁ pāramī jala nidhiṁ caturiha bāhu,",
  "anya-e0607n:388_0": "Sattīhi suttari ciraṁ janakova sindhuṁ;",
  "anya-e0607n:389_0": "Sampanna vikkama phalosi yathā casova,",
  "anya-e0607n:390_0": "Evaṁ janā viriyatapphalame dhayantu.",
  "anya-e0607n:391_0": "78.",
  "anya-e0607n:392_0": "Satta paradha dahanesu ciraṁ sudhantaṁ,",
  "anya-e0607n:393_0": "Khantī suvaṇṇa kata rūpa samantimattā;",
  "anya-e0607n:394_0": "Sabbā parādhamasahi tvaṁasayhamevaṁ,",
  "anya-e0607n:395_0": "Sabbe janāpi khamanena bhajantu santiṁ.",
  "anya-e0607n:396_0": "79.",
  "anya-e0607n:397_0": "Lakkhādhikaṁ catura saṁkhiya kappa kālaṁ,",
  "anya-e0607n:398_0": "Saccena suṭṭhu paribhāvita vācino te;",
  "anya-e0607n:399_0": "Vācāya sacca phusitāya samenti jantu,",
  "anya-e0607n:400_0": "Evaṁ visuddha vacanā janatā bhavantu.",
  "anya-e0607n:401_0": "80.",
  "anya-e0607n:402_0": "Ādinna dhamma mahiyatthira suppatiṭṭhā,",
  "anya-e0607n:403_0": "Dhiṭṭhāna pārami mahā vajiraddi tuyhaṁ;",
  "anya-e0607n:404_0": "Sattena kena pi yathāhi abhejja nejjo,",
  "anya-e0607n:405_0": "Evaṁ janāpi kusalesu adhiṭṭha hantu.",
  "anya-e0607n:406_0": "81.",
  "anya-e0607n:407_0": "Tvaṁ sabba satta cirabhāvita metta citta,",
  "anya-e0607n:408_0": "Toyehi saṁsamita kodha mahā hutāso;",
  "anya-e0607n:409_0": "Lokuttaraṁ taditaraṁ hitamāvahittho,",
  "anya-e0607n:410_0": "Evaṁ janesu janatā hitamāvahantū.",
  "anya-e0607n:411_0": "82.",
  "anya-e0607n:412_0": "Mittopakāra paṭipakkha janāpakāre,",
  "anya-e0607n:413_0": "Tvaṁ nibbikāra manaso cirabhāvanāya;",
  "anya-e0607n:414_0": "Pattosilābha pabhutaṭṭusu nibbikāraṁ,",
  "anya-e0607n:415_0": "Evaṁ janānunaya kopa nudā bhavantu.",
  "anya-e0607n:416_0": "83.",
  "anya-e0607n:417_0": "Sampanna hetu vibhavo tusite vimānaṁ,",
  "anya-e0607n:418_0": "Yuttaṁ guṇehi navabhippadavī vimānaṁ;",
  "anya-e0607n:419_0": "Tvaṁ vādhiparamidhirohiniyā tiloko,",
  "anya-e0607n:420_0": "Ārohatu bhaya sukhaṁ padavī vimānaṁ.",
  "anya-e0607n:421_0": "84.",
  "anya-e0607n:422_0": "Tvaṁverahaṁsi samabujjhi yathāca sammā,",
  "anya-e0607n:423_0": "Sampanna vijja caraṇo sugatosi hontu;",
  "anya-e0607n:424_0": "Lokaṁ vido purisadammasusārathī si,",
  "anya-e0607n:425_0": "Satthāsi bujjhi bhagavā si tatheva jantu.",
  "anya-e0607n:426_0": "85.",
  "anya-e0607n:427_0": "Saccitta bhū nidahitaṁ janatāya tuyhaṁ,",
  "anya-e0607n:428_0": "Kalyāṇavaṇṇaratanaṇṇavajātibhinnaṁ;",
  "anya-e0607n:429_0": "Dukkhaggi cora jalupaddutajāti gehe,",
  "anya-e0607n:430_0": "Tassā sukhaṁ bhavatu jīvitumāpadāya.",
  "anya-e0607n:431_0": "86.",
  "anya-e0607n:432_0": "Vācā vicitta vara tantu gataṅgi kaṇṭhe,",
  "anya-e0607n:433_0": "Svā mutta sagguṇa mahā ratanā valī te;",
  "anya-e0607n:434_0": "Vevaṇṇi yattani bhavaṁ sakalampahāya,",
  "anya-e0607n:435_0": "Hotañjanassa siri saṅgama maṅgalāya.",
  "anya-e0607n:436_0": "87.",
  "anya-e0607n:437_0": "Taṁ sagguṇatthava dahaṭṭha sutippanāli,",
  "anya-e0607n:438_0": "Nissandamāna guṇanīra nipāna tinte;",
  "anya-e0607n:439_0": "Khettetta saññini janā kata loma haṁsa,",
  "anya-e0607n:440_0": "Bījankurī kusala sassa phalaṁ labhantu.",
  "anya-e0607n:441_0": "88.",
  "anya-e0607n:442_0": "Āpāyikappabhuti dukkha nidāgha kāla,",
  "anya-e0607n:443_0": "Santāpitā nikhila loka mano kadambā;",
  "anya-e0607n:444_0": "Taṁ vaṅṇa megha phusanā hasanaṅkurehi,",
  "anya-e0607n:445_0": "Iddhā bhavantu mati vallari vellītā te.",
  "anya-e0607n:446_0": "89.",
  "anya-e0607n:447_0": "Hetuddasā phaladasā samavaṭṭhitaṁ taṁ,",
  "anya-e0607n:448_0": "Sabbattha satta hitamāvahaṇena siddhaṁ;",
  "anya-e0607n:449_0": "Cintāpathātiganubhāva vibhāvanante,",
  "anya-e0607n:450_0": "Bhūtānamatthu caritabbhutamattha sidhyā.",
  "anya-e0607n:451_0": "90.",
  "anya-e0607n:452_0": "Aṅgārakāsumabhilaṅghiya dāna kāle,",
  "anya-e0607n:453_0": "Bhattattano pada paṭicchaka paṅkajā ca;",
  "anya-e0607n:454_0": "Yātakkhaṇe tava pade dhaṭamuṭṭhahitvā,",
  "anya-e0607n:455_0": "Paṅkeruhāṁ siva madhuṁ sarataṁ dadantu.",
  "anya-e0607n:456_0": "91.",
  "anya-e0607n:457_0": "Saccena maccha pati vassita vassadhārā,",
  "anya-e0607n:458_0": "Satte dayāya tava vassita vassadhārā;",
  "anya-e0607n:459_0": "Gimhe janassa samayiṁsu yathā tathātā,",
  "anya-e0607n:460_0": "Dhamambuvuṭṭhiva samentu kilesa dāhe.",
  "anya-e0607n:461_0": "92.",
  "anya-e0607n:462_0": "Chaddanta nāga patinā khamatā parādhaṁ,",
  "anya-e0607n:463_0": "Chetvā kare ṭhapita dantavarāva luddaṁ;",
  "anya-e0607n:464_0": "Loke hitāya ṭhapitā tava danta dhātu,",
  "anya-e0607n:465_0": "Seṭṭhā janaṁ siva puraṁ lahu pāpayantu.",
  "anya-e0607n:466_0": "93.",
  "anya-e0607n:467_0": "Taṁ temiyākhya yatinossama mālakamhi,",
  "anya-e0607n:468_0": "Okiṇṇa mutta kanakā vuja vippakiṇṇā;",
  "anya-e0607n:469_0": "Kāruñña vārida cuto daka bindu bandhū,",
  "anya-e0607n:470_0": "Dhātu samentu tava jantusu dukkhadāhe.",
  "anya-e0607n:471_0": "94.",
  "anya-e0607n:472_0": "Raṭṭhassa attha caraṇāya asammukhassa,",
  "anya-e0607n:473_0": "Rāmena dinna tiṇa saṁkhata pādukāva;",
  "anya-e0607n:474_0": "Bhuttā tayā ciramasammukha nāgatassa,",
  "anya-e0607n:475_0": "Lokassa atthamanu tiṭṭhatu patta dhātu.",
  "anya-e0607n:476_0": "95.",
  "anya-e0607n:477_0": "Vutto janānamupadissa varāha raññā,",
  "anya-e0607n:478_0": "Satthiṁ sahassa saradaṁ viya ñāya dhammo;",
  "anya-e0607n:479_0": "Ādeyya heyyamupadissa tayā pavutto,",
  "anya-e0607n:480_0": "Dhammo pavattatu ciraṁ janatā hitāya.",
  "anya-e0607n:481_0": "96.",
  "anya-e0607n:482_0": "Mārāri maddana hitādhigamaṁ karotā,",
  "anya-e0607n:483_0": "Bhatto tayā vara mahā jaya bodhi rājā;",
  "anya-e0607n:484_0": "Saggā pavavagga hitahetu janassa hantvā,",
  "anya-e0607n:485_0": "Sabbantarāyamiha tiṭṭhatu suṭṭhu sajjo.",
  "anya-e0607n:486_0": "97.",
  "anya-e0607n:487_0": "Sāmoda vaṇṇa bhajanī guṇa mañjarīyaṁ,",
  "anya-e0607n:488_0": "Cariyā latā vikasitā tava sapphalaṅgaṁ;",
  "anya-e0607n:489_0": "Okiṇṇa citta madhupe rasa pīṇayanti,",
  "anya-e0607n:490_0": "Sambhāvitā bhuvi pavattatu matthakehi.",
  "anya-e0607n:491_0": "98.",
  "anya-e0607n:492_0": "Sambuddha selavalayantara jānanavhā,",
  "anya-e0607n:493_0": "Nottattato tipathagā yati sāgaraṭṭhā;",
  "anya-e0607n:494_0": "Dhammā pagā suti vase tarite puṇanti,",
  "anya-e0607n:495_0": "Sambhāra sassamiha vattatu pacayanti.",
  "anya-e0607n:496_0": "99.",
  "anya-e0607n:497_0": "Paññāṇa kūpa sita paggaha vāyu gāhī,",
  "anya-e0607n:498_0": "Saddhā lakāra sahitā sati pota vāhā;",
  "anya-e0607n:499_0": "Sampāpayātu bhava sāgara pāra tīra,",
  "anya-e0607n:500_0": "Sappattanaṁ varadhane pati patti nāvā.",
  "anya-e0607n:501_0": "100.",
  "anya-e0607n:502_0": "Bojjhaṅga satta ratanākara dhamma khandha,",
  "anya-e0607n:503_0": "Gambhīra nīra caya sāsana sāgaro saṁ;",
  "anya-e0607n:504_0": "So sīlyananta tanu veṭitha ñāṇa mantha,",
  "anya-e0607n:505_0": "Selena manthitavataṁ disatā mataṁ ve.",
  "anya-e0607n:506_0": "101.",
  "anya-e0607n:507_0": "Vuttena tena vidhinā vidhinā tato taṁ,",
  "anya-e0607n:508_0": "Laddhā nubhūtamamataṁ khila dosa nāsaṁ;",
  "anya-e0607n:509_0": "Accanta roga jaratā maraṇā bhi bhūtaṁ,",
  "anya-e0607n:510_0": "Bhūtaṁ karotu amaraṁ ajaraṁ arogaṁ.",
  "anya-e0607n:511_0": "102.",
  "anya-e0607n:512_0": "Saddhamma rāja raviniggata dhammaraṁsi,",
  "anya-e0607n:513_0": "Phullo dhutaṅgadala saṁvara kesarāli;",
  "anya-e0607n:514_0": "Saṅghāravinda nikaro samadhuṁ samādhi,",
  "anya-e0607n:515_0": "Sakkiṇṇiko disatu sāsana vāpi jato.",
  "anya-e0607n:516_0": "103.",
  "anya-e0607n:517_0": "Ānanda rañña ratanādi mahā yatinda,",
  "anya-e0607n:518_0": "Niccappabuddha padumappiya sevinaṅgī;",
  "anya-e0607n:519_0": "Buddhappiyena ghana buddha guṇappiyena,",
  "anya-e0607n:520_0": "Therālinā racita pajjamadhuṁ pibantu.",
  "anya-e0607n:521_0": "104.",
  "anya-e0607n:522_0": "Itthaṁ rūpa guṇānukittanavasā taṁ taṁ hitā siṁ sato,",
  "anya-e0607n:523_0": "Vatthānussati vattita iha yathā sattesu mettā ca me;",
  "anya-e0607n:524_0": "Evaṁ tābhi bhavanta ruttara tarā vattantu tā bodhi me,",
  "anya-e0607n:525_0": "Saṁyogoca dhanehi santihi bhave kalyāna mittehi ca."
}