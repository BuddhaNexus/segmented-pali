{
  "ja108:0.1_0": "Jātaka",
  "ja108:0.2_0": "Ekakanipāta",
  "ja108:0.3_0": "Parosatavagga",
  "ja108:0.4_0": "8. Bāhiyajātaka",
  "ja108:1.1_0": "“Sikkheyya sikkhitabbāni,",
  "ja108:1.2_0": "santi sacchandino janā;",
  "ja108:1.3_0": "Bāhiyā hi suhannena,",
  "ja108:1.4_0": "rājānamabhirādhayī”ti.",
  "ja108:2.1_0": "Bāhiyajātakaṁ aṭṭhamaṁ."
}