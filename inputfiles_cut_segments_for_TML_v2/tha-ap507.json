{
  "tha-ap507:0.1_0": "Therāpadāna",
  "tha-ap507:0.2_0": "Kaṇikāravagga",
  "tha-ap507:0.3_0": "5 Pādaphaliyattheraapadāna",
  "tha-ap507:1.1_0": "“Suvaṇṇavaṇṇaṁ sambuddhaṁ,",
  "tha-ap507:1.2_0": "Āhutīnaṁ paṭiggahaṁ;",
  "tha-ap507:1.3_0": "Rathiyaṁ paṭipajjantaṁ,",
  "tha-ap507:1.4_0": "Pādaphalaṁ adāsahaṁ.",
  "tha-ap507:2.1_0": "Ekanavutito kappe,",
  "tha-ap507:2.2_0": "yaṁ phalaṁ adadiṁ tadā;",
  "tha-ap507:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap507:2.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap507:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap507:3.2_0": "…pe…",
  "tha-ap507:3.3_0": "viharāmi anāsavo.",
  "tha-ap507:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap507:4.2_0": "…pe…",
  "tha-ap507:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap507:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap507:5.2_0": "…pe…",
  "tha-ap507:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap507:6.1_0": "Itthaṁ sudaṁ āyasmā pādaphaliyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap507:7.1_0": "Pādaphaliyattherassāpadānaṁ pañcamaṁ."
}