{
  "ja353:0.1_0": "Jātaka",
  "ja353:0.2_0": "Pañcakanipāta",
  "ja353:0.3_0": "Maṇikuṇḍalavagga",
  "ja353:0.4_0": "3. Venasākhajātaka",
  "ja353:1.1_0": "“Nayidaṁ niccaṁ bhavitabbaṁ brahmadatta,",
  "ja353:1.2_0": "Khemaṁ subhikkhaṁ sukhatā ca kāye;",
  "ja353:1.3_0": "Atthaccaye mā ahu sampamūḷho,",
  "ja353:1.4_0": "Bhinnaplavo sāgarasseva majjhe.",
  "ja353:2.1_0": "Yāni karoti puriso,",
  "ja353:2.2_0": "tāni attani passati;",
  "ja353:2.3_0": "Kalyāṇakārī kalyāṇaṁ,",
  "ja353:2.4_0": "pāpakārī ca pāpakaṁ;",
  "ja353:2.5_0": "Yādisaṁ vapate bījaṁ,",
  "ja353:2.6_0": "tādisaṁ harate phalaṁ”.",
  "ja353:3.1_0": "“Idaṁ tadācariyavaco,",
  "ja353:3.2_0": "pārāsariyo yadabravi;",
  "ja353:3.3_0": "Mā su tvaṁ akari pāpaṁ,",
  "ja353:3.4_0": "yaṁ tvaṁ pacchā kataṁ tape.",
  "ja353:4.1_0": "Ayameva so piṅgiya venasākho,",
  "ja353:4.2_0": "Yamhi ghātayiṁ khattiyānaṁ sahassaṁ;",
  "ja353:4.3_0": "Alaṅkate candanasārānulitte,",
  "ja353:4.4_0": "Tameva dukkhaṁ paccāgataṁ mamaṁ.",
  "ja353:5.1_0": "Sāmā ca kho candanalittagattā,",
  "ja353:5.2_0": "Laṭṭhīva sobhañjanakassa uggatā;",
  "ja353:5.3_0": "Adisvā kālaṁ karissāmi ubbariṁ,",
  "ja353:5.4_0": "Taṁ me ito dukkhataraṁ bhavissatī”ti.",
  "ja353:6.1_0": "Venasākhajātakaṁ tatiyaṁ."
}