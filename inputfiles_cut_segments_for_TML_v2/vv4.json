{
  "vv4:0.1_0": "Vimānavatthu",
  "vv4:0.2_0": "Itthivimāna",
  "vv4:0.3_0": "Pīṭhavagga",
  "vv4:0.4_0": "4. Catutthapīṭhavimānavatthu",
  "vv4:1.1_0": "“Pīṭhaṁ te veḷuriyamayaṁ uḷāraṁ,",
  "vv4:1.2_0": "Manojavaṁ gacchati yenakāmaṁ;",
  "vv4:1.3_0": "Alaṅkate malyadhare suvatthe,",
  "vv4:1.4_0": "Obhāsasi vijjurivabbhakūṭaṁ.",
  "vv4:2.1_0": "Kena tetādiso vaṇṇo,",
  "vv4:2.2_0": "kena te idha mijjhati;",
  "vv4:2.3_0": "Uppajjanti ca te bhogā,",
  "vv4:2.4_0": "ye keci manaso piyā.",
  "vv4:3.1_0": "Pucchāmi taṁ devi mahānubhāve,",
  "vv4:3.2_0": "Manussabhūtā kimakāsi puññaṁ;",
  "vv4:3.3_0": "Kenāsi evaṁ jalitānubhāvā,",
  "vv4:3.4_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv4:4.1_0": "Sā devatā attamanā,",
  "vv4:4.2_0": "moggallānena pucchitā;",
  "vv4:4.3_0": "Pañhaṁ puṭṭhā viyākāsi,",
  "vv4:4.4_0": "yassa kammassidaṁ phalaṁ.",
  "vv4:5.1_0": "“Appassa kammassa phalaṁ mamedaṁ,",
  "vv4:5.2_0": "Yenamhi evaṁ jalitānubhāvā;",
  "vv4:5.3_0": "Ahaṁ manussesu manussabhūtā,",
  "vv4:5.4_0": "Purimāya jātiyā manussaloke.",
  "vv4:6.1_0": "Addasaṁ virajaṁ bhikkhuṁ,",
  "vv4:6.2_0": "vippasannamanāvilaṁ;",
  "vv4:6.3_0": "Tassa adāsahaṁ pīṭhaṁ,",
  "vv4:6.4_0": "pasannā sehi pāṇibhi.",
  "vv4:7.1_0": "Tena metādiso vaṇṇo,",
  "vv4:7.2_0": "tena me idha mijjhati;",
  "vv4:7.3_0": "Uppajjanti ca me bhogā,",
  "vv4:7.4_0": "ye keci manaso piyā.",
  "vv4:8.1_0": "Akkhāmi te bhikkhu mahānubhāva,",
  "vv4:8.2_0": "Manussabhūtā yamakāsi puññaṁ;",
  "vv4:8.3_0": "Tenamhi evaṁ jalitānubhāvā,",
  "vv4:8.4_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv4:9.1_0": "Catutthapīṭhavimānaṁ catutthaṁ."
}