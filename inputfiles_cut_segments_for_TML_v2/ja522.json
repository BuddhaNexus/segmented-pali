{
  "ja522:0.1_0": "Jātaka",
  "ja522:0.2_0": "Cattālīsanipāta",
  "ja522:0.3_0": "Tesakuṇavagga",
  "ja522:0.4_0": "2. Sarabhaṅgajātaka",
  "ja522:1.1_0": "“Alaṅkatā kuṇḍalino suvatthā,",
  "ja522:1.2_0": "Veḷuriyamuttātharukhaggabandhā;",
  "ja522:1.3_0": "Rathesabhā tiṭṭhatha ke nu tumhe,",
  "ja522:1.4_0": "Kathaṁ vo jānanti manussaloke”.",
  "ja522:2.1_0": "“Ahamaṭṭhako bhīmaratho panāyaṁ,",
  "ja522:2.2_0": "Kāliṅgarājā pana uggatoyaṁ;",
  "ja522:2.3_0": "Susaññatānaṁ isīnaṁ dassanāya,",
  "ja522:2.4_0": "Idhāgatā pucchitāyemha pañhe”.",
  "ja522:3.1_0": "“Vehāyasaṁ tiṭṭhasi antalikkhe,",
  "ja522:3.2_0": "Pathaddhuno pannaraseva cando;",
  "ja522:3.3_0": "Pucchāmi taṁ yakkha mahānubhāva,",
  "ja522:3.4_0": "Kathaṁ taṁ jānanti manussaloke”.",
  "ja522:4.1_0": "“Yamāhu devesu sujampatīti,",
  "ja522:4.2_0": "Maghavāti taṁ āhu manussaloke;",
  "ja522:4.3_0": "Sa devarājā idamajja patto,",
  "ja522:4.4_0": "Susaññatānaṁ isīnaṁ dassanāya”.",
  "ja522:5.1_0": "“Dūre sutā no isayo samāgatā,",
  "ja522:5.2_0": "Mahiddhikā iddhiguṇūpapannā;",
  "ja522:5.3_0": "Vandāmi te ayire pasannacitto,",
  "ja522:5.4_0": "Ye jīvalokettha manussaseṭṭhā”.",
  "ja522:6.1_0": "“Gandho isīnaṁ ciradikkhitānaṁ,",
  "ja522:6.2_0": "Kāyā cuto gacchati mālutena;",
  "ja522:6.3_0": "Ito paṭikkamma sahassanetta,",
  "ja522:6.4_0": "Gandho isīnaṁ asuci devarāja”.",
  "ja522:7.1_0": "“Gandho isīnaṁ ciradikkhitānaṁ,",
  "ja522:7.2_0": "Kāyā cuto gacchatu mālutena;",
  "ja522:7.3_0": "Vicitrapupphaṁ surabhiṁva mālaṁ,",
  "ja522:7.4_0": "Gandhañca etaṁ pāṭikaṅkhāma bhante;",
  "ja522:7.5_0": "Na hettha devā paṭikkūlasaññino”.",
  "ja522:8.1_0": "“Purindado bhūtapatī yasassī,",
  "ja522:8.2_0": "Devānamindo sakko maghavā sujampati;",
  "ja522:8.3_0": "Sa devarājā asuragaṇappamaddano,",
  "ja522:8.4_0": "Okāsamākaṅkhati pañha pucchituṁ.",
  "ja522:9.1_0": "Ko nevimesaṁ idha paṇḍitānaṁ,",
  "ja522:9.2_0": "Pañhe puṭṭho nipuṇe byākarissati;",
  "ja522:9.3_0": "Tiṇṇañca raññaṁ manujādhipānaṁ,",
  "ja522:9.4_0": "Devānamindassa ca vāsavassa”.",
  "ja522:10.1_0": "“Ayaṁ isi sarabhaṅgo tapassī,",
  "ja522:10.2_0": "Yato jāto virato methunasmā;",
  "ja522:10.3_0": "Āceraputto suvinītarūpo,",
  "ja522:10.4_0": "So nesaṁ pañhāni viyākarissati”.",
  "ja522:11.1_0": "“Koṇḍañña pañhāni viyākarohi,",
  "ja522:11.2_0": "Yācanti taṁ isayo sādhurūpā;",
  "ja522:11.3_0": "Koṇḍañña eso manujesu dhammo,",
  "ja522:11.4_0": "Yaṁ vuddhamāgacchati esa bhāro”.",
  "ja522:12.1_0": "“Katāvakāsā pucchantu bhonto,",
  "ja522:12.2_0": "Yaṁ kiñci pañhaṁ manasābhipatthitaṁ;",
  "ja522:12.3_0": "Ahañhi taṁ taṁ vo viyākarissaṁ,",
  "ja522:12.4_0": "Ñatvā sayaṁ lokamimaṁ parañca”.",
  "ja522:13.1_0": "Tato ca maghavā sakko,",
  "ja522:13.2_0": "atthadassī purindado;",
  "ja522:13.3_0": "Apucchi paṭhamaṁ pañhaṁ,",
  "ja522:13.4_0": "yañcāsi abhipatthitaṁ.",
  "ja522:14.1_0": "“Kiṁ sū vadhitvā na kadāci socati,",
  "ja522:14.2_0": "Kissappahānaṁ isayo vaṇṇayanti;",
  "ja522:14.3_0": "Kassīdha vuttaṁ pharusaṁ khametha,",
  "ja522:14.4_0": "Akkhāhi me koṇḍañña etamatthaṁ”.",
  "ja522:15.1_0": "“Kodhaṁ vadhitvā na kadāci socati,",
  "ja522:15.2_0": "Makkhappahānaṁ isayo vaṇṇayanti;",
  "ja522:15.3_0": "Sabbesaṁ vuttaṁ pharusaṁ khametha,",
  "ja522:15.4_0": "Etaṁ khantiṁ uttamamāhu santo”.",
  "ja522:16.1_0": "“Sakkā ubhinnaṁ vacanaṁ titikkhituṁ,",
  "ja522:16.2_0": "Sadisassa vā seṭṭhatarassa vāpi;",
  "ja522:16.3_0": "Kathaṁ nu hīnassa vaco khametha,",
  "ja522:16.4_0": "Akkhāhi me koṇḍañña etamatthaṁ”.",
  "ja522:17.1_0": "“Bhayā hi seṭṭhassa vaco khametha,",
  "ja522:17.2_0": "Sārambhahetū pana sādisassa;",
  "ja522:17.3_0": "Yo cīdha hīnassa vaco khametha,",
  "ja522:17.4_0": "Etaṁ khantiṁ uttamamāhu santo”.",
  "ja522:18.1_0": "“Kathaṁ vijaññā catupattharūpaṁ,",
  "ja522:18.2_0": "Seṭṭhaṁ sarikkhaṁ athavāpi hīnaṁ;",
  "ja522:18.3_0": "Virūparūpena caranti santo,",
  "ja522:18.4_0": "Tasmā hi sabbesaṁ vaco khametha”.",
  "ja522:19.1_0": "“Na hetamatthaṁ mahatīpi senā,",
  "ja522:19.2_0": "Sarājikā yujjhamānā labhetha;",
  "ja522:19.3_0": "Yaṁ khantimā sappuriso labhetha,",
  "ja522:19.4_0": "Khantī balassūpasamanti verā”.",
  "ja522:20.1_0": "“Subhāsitaṁ te anumodiyāna,",
  "ja522:20.2_0": "Aññaṁ taṁ pucchāmi tadiṅgha brūhi;",
  "ja522:20.3_0": "Yathā ahuṁ daṇḍakī nāḷikero,",
  "ja522:20.4_0": "Athajjuno kalābu cāpi rājā;",
  "ja522:20.5_0": "Tesaṁ gatiṁ brūhi supāpakamminaṁ,",
  "ja522:20.6_0": "Katthūpapannā isinaṁ viheṭhakā”.",
  "ja522:21.1_0": "“Kisañhi vacchaṁ avakiriya daṇḍakī,",
  "ja522:21.2_0": "Ucchinnamūlo sajano saraṭṭho;",
  "ja522:21.3_0": "Kukkuḷanāme nirayamhi paccati,",
  "ja522:21.4_0": "Tassa phuliṅgāni patanti kāye.",
  "ja522:22.1_0": "Yo saññate pabbajite aheṭhayi,",
  "ja522:22.2_0": "Dhammaṁ bhaṇante samaṇe adūsake;",
  "ja522:22.3_0": "Taṁ nāḷikeraṁ sunakhā parattha,",
  "ja522:22.4_0": "Saṅgamma khādanti viphandamānaṁ.",
  "ja522:23.1_0": "Athajjuno niraye sattisūle,",
  "ja522:23.2_0": "Avaṁsiro patito uddhampādo;",
  "ja522:23.3_0": "Aṅgīrasaṁ gotamaṁ heṭhayitvā,",
  "ja522:23.4_0": "Khantiṁ tapassiṁ cirabrahmacāriṁ.",
  "ja522:24.1_0": "Yo khaṇḍaso pabbajitaṁ achedayi,",
  "ja522:24.2_0": "Khantiṁ vadantaṁ samaṇaṁ adūsakaṁ;",
  "ja522:24.3_0": "Kalābuvīciṁ upapajja paccati,",
  "ja522:24.4_0": "Mahāpatāpaṁ kaṭukaṁ bhayānakaṁ.",
  "ja522:25.1_0": "Etāni sutvā nirayāni paṇḍito,",
  "ja522:25.2_0": "Aññāni pāpiṭṭhatarāni cettha;",
  "ja522:25.3_0": "Dhammaṁ care samaṇabrāhmaṇesu,",
  "ja522:25.4_0": "Evaṅkaro saggamupeti ṭhānaṁ”.",
  "ja522:26.1_0": "“Subhāsitaṁ te anumodiyāna,",
  "ja522:26.2_0": "Aññaṁ taṁ pucchāmi tadiṅgha brūhi;",
  "ja522:26.3_0": "Kathaṁvidhaṁ sīlavantaṁ vadanti,",
  "ja522:26.4_0": "Kathaṁvidhaṁ paññavantaṁ vadanti;",
  "ja522:26.5_0": "Kathaṁvidhaṁ sappurisaṁ vadanti,",
  "ja522:26.6_0": "Kathaṁvidhaṁ no siri no jahāti”.",
  "ja522:27.1_0": "“Kāyena vācāya ca yodha saññato,",
  "ja522:27.2_0": "Manasā ca kiñci na karoti pāpaṁ;",
  "ja522:27.3_0": "Na attahetū alikaṁ bhaṇeti,",
  "ja522:27.4_0": "Tathāvidhaṁ sīlavantaṁ vadanti.",
  "ja522:28.1_0": "Gambhīrapañhaṁ manasābhicintayaṁ,",
  "ja522:28.2_0": "Nāccāhitaṁ kamma karoti luddaṁ;",
  "ja522:28.3_0": "Kālāgataṁ atthapadaṁ na riñcati,",
  "ja522:28.4_0": "Tathāvidhaṁ paññavantaṁ vadanti.",
  "ja522:29.1_0": "Yo ve kataññū katavedi dhīro,",
  "ja522:29.2_0": "Kalyāṇamitto daḷhabhatti ca hoti;",
  "ja522:29.3_0": "Dukhitassa sakkacca karoti kiccaṁ,",
  "ja522:29.4_0": "Tathāvidhaṁ sappurisaṁ vadanti.",
  "ja522:30.1_0": "Etehi sabbehi guṇehupeto,",
  "ja522:30.2_0": "Saddho mudū saṁvibhāgī vadaññū;",
  "ja522:30.3_0": "Saṅgāhakaṁ sakhilaṁ saṇhavācaṁ,",
  "ja522:30.4_0": "Tathāvidhaṁ no siri no jahāti”.",
  "ja522:31.1_0": "“Subhāsitaṁ te anumodiyāna,",
  "ja522:31.2_0": "Aññaṁ taṁ pucchāmi tadiṅgha brūhi;",
  "ja522:31.3_0": "Sīlaṁ siriñcāpi satañca dhammaṁ,",
  "ja522:31.4_0": "Paññañca kaṁ seṭṭhataraṁ vadanti”.",
  "ja522:32.1_0": "“Paññā hi seṭṭhā kusalā vadanti,",
  "ja522:32.2_0": "Nakkhattarājāriva tārakānaṁ;",
  "ja522:32.3_0": "Sīlaṁ sīrī cāpi satañca dhammo,",
  "ja522:32.4_0": "Anvāyikā paññavato bhavanti”.",
  "ja522:33.1_0": "“Subhāsitaṁ te anumodiyāna,",
  "ja522:33.2_0": "Aññaṁ taṁ pucchāmi tadiṅgha brūhi;",
  "ja522:33.3_0": "Kathaṅkaro kintikaro kimācaraṁ,",
  "ja522:33.4_0": "Kiṁ sevamāno labhatīdha paññaṁ;",
  "ja522:33.5_0": "Paññāya dāni paṭipadaṁ vadehi,",
  "ja522:33.6_0": "Kathaṅkaro paññavā hoti macco”.",
  "ja522:34.1_0": "“Sevetha vuddhe nipuṇe bahussute,",
  "ja522:34.2_0": "Uggāhako ca paripucchako siyā;",
  "ja522:34.3_0": "Suṇeyya sakkacca subhāsitāni,",
  "ja522:34.4_0": "Evaṅkaro paññavā hoti macco.",
  "ja522:35.1_0": "Sa paññavā kāmaguṇe avekkhati,",
  "ja522:35.2_0": "Aniccato dukkhato rogato ca;",
  "ja522:35.3_0": "Evaṁ vipassī pajahāti chandaṁ,",
  "ja522:35.4_0": "Dukkhesu kāmesu mahabbhayesu.",
  "ja522:36.1_0": "Sa vītarāgo pavineyya dosaṁ,",
  "ja522:36.2_0": "Mettaṁ cittaṁ bhāvaye appamāṇaṁ;",
  "ja522:36.3_0": "Sabbesu bhūtesu nidhāya daṇḍaṁ,",
  "ja522:36.4_0": "Anindito brahmamupeti ṭhānaṁ”.",
  "ja522:37.1_0": "“Mahatthiyaṁ āgamanaṁ ahosi,",
  "ja522:37.2_0": "Tavamaṭṭhakā bhīmarathassa cāpi;",
  "ja522:37.3_0": "Kāliṅgarājassa ca uggatassa,",
  "ja522:37.4_0": "Sabbesa vo kāmarāgo pahīno”.",
  "ja522:38.1_0": "“Evametaṁ paracittavedi,",
  "ja522:38.2_0": "Sabbesa no kāmarāgo pahīno;",
  "ja522:38.3_0": "Karohi okāsamanuggahāya,",
  "ja522:38.4_0": "Yathā gatiṁ te abhisambhavema”.",
  "ja522:39.1_0": "“Karomi okāsamanuggahāya,",
  "ja522:39.2_0": "Tathā hi vo kāmarāgo pahīno;",
  "ja522:39.3_0": "Pharātha kāyaṁ vipulāya pītiyā,",
  "ja522:39.4_0": "Yathā gatiṁ me abhisambhavetha”.",
  "ja522:40.1_0": "“Sabbaṁ karissāma tavānusāsaniṁ,",
  "ja522:40.2_0": "Yaṁ yaṁ tuvaṁ vakkhasi bhūripañña;",
  "ja522:40.3_0": "Pharāma kāyaṁ vipulāya pītiyā,",
  "ja522:40.4_0": "Yathā gatiṁ te abhisambhavema”.",
  "ja522:41.1_0": "“Katāya vacchassa kisassa pūjā,",
  "ja522:41.2_0": "Gacchantu bhonto isayo sādhurūpā;",
  "ja522:41.3_0": "Jhāne ratā hotha sadā samāhitā,",
  "ja522:41.4_0": "Esā ratī pabbajitassa seṭṭhā”.",
  "ja522:42.1_0": "Sutvāna gāthā paramatthasaṁhitā,",
  "ja522:42.2_0": "Subhāsitā isinā paṇḍitena;",
  "ja522:42.3_0": "Te vedajātā anumodamānā,",
  "ja522:42.4_0": "Pakkāmu devā devapuraṁ yasassino.",
  "ja522:43.1_0": "Gāthā imā atthavatī subyañjanā,",
  "ja522:43.2_0": "Subhāsitā isinā paṇḍitena;",
  "ja522:43.3_0": "Yo kocimā aṭṭhikatvā suṇeyya,",
  "ja522:43.4_0": "Labhetha pubbāpariyaṁ visesaṁ;",
  "ja522:43.5_0": "Laddhāna pubbāpariyaṁ visesaṁ,",
  "ja522:43.6_0": "Adassanaṁ maccurājassa gacche.",
  "ja522:44.1_0": "“Sālissaro sāriputto,",
  "ja522:44.2_0": "meṇḍissaro ca kassapo;",
  "ja522:44.3_0": "Pabbato anuruddho ca,",
  "ja522:44.4_0": "kaccāyano ca devalo.",
  "ja522:45.1_0": "Anusisso ca ānando,",
  "ja522:45.2_0": "kisavaccho ca kolito;",
  "ja522:45.3_0": "Nārado udāyitthero,",
  "ja522:45.4_0": "parisā buddhaparisā;",
  "ja522:45.5_0": "Sarabhaṅgo lokanātho,",
  "ja522:45.6_0": "evaṁ dhāretha jātakan”ti.",
  "ja522:46.1_0": "Sarabhaṅgajātakaṁ dutiyaṁ."
}