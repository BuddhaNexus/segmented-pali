{
  "ud3.9:0.1_0": "Udāna 3.9",
  "ud3.9:0.2_0": "Sippasutta",
  "ud3.9:1.1_0": "Evaṁ me sutaṁ—",
  "ud3.9:1.2_0": "ekaṁ samayaṁ bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "ud3.9:1.3_0": "Tena kho pana samayena sambahulānaṁ bhikkhūnaṁ pacchābhattaṁ piṇḍapātapaṭikkantānaṁ maṇḍalamāḷe sannisinnānaṁ sannipatitānaṁ ayamantarākathā udapādi:",
  "ud3.9:1.4_0": "“ko nu kho, āvuso, sippaṁ jānāti?",
  "ud3.9:1.5_0": "Ko kiṁ sippaṁ sikkhi?",
  "ud3.9:1.6_0": "Kataraṁ sippaṁ sippānaṁ aggan”ti?",
  "ud3.9:2.1_0": "Tatthekacce evamāhaṁsu:",
  "ud3.9:2.2_0": "“hatthisippaṁ sippānaṁ aggan”ti.",
  "ud3.9:2.3_0": "Ekacce evamāhaṁsu:",
  "ud3.9:2.4_0": "“assasippaṁ sippānaṁ aggan”ti.",
  "ud3.9:2.5_0": "Ekacce evamāhaṁsu:",
  "ud3.9:2.6_0": "“rathasippaṁ sippānaṁ aggan”ti.",
  "ud3.9:2.7_0": "Ekacce evamāhaṁsu:",
  "ud3.9:2.8_0": "“dhanusippaṁ sippānaṁ aggan”ti.",
  "ud3.9:2.9_0": "Ekacce evamāhaṁsu:",
  "ud3.9:2.10_0": "“tharusippaṁ sippānaṁ aggan”ti.",
  "ud3.9:2.11_0": "Ekacce evamāhaṁsu:",
  "ud3.9:2.12_0": "“muddāsippaṁ sippānaṁ aggan”ti.",
  "ud3.9:2.13_0": "Ekacce evamāhaṁsu:",
  "ud3.9:2.14_0": "“gaṇanāsippaṁ sippānaṁ aggan”ti.",
  "ud3.9:2.15_0": "Ekacce evamāhaṁsu:",
  "ud3.9:2.16_0": "“saṅkhānasippaṁ sippānaṁ aggan”ti.",
  "ud3.9:2.17_0": "Ekacce evamāhaṁsu:",
  "ud3.9:2.18_0": "“lekhāsippaṁ sippānaṁ aggan”ti.",
  "ud3.9:2.19_0": "Ekacce evamāhaṁsu:",
  "ud3.9:2.20_0": "“kāveyyasippaṁ sippānaṁ aggan”ti.",
  "ud3.9:2.21_0": "Ekacce evamāhaṁsu:",
  "ud3.9:2.22_0": "“lokāyatasippaṁ sippānaṁ aggan”ti.",
  "ud3.9:2.23_0": "Ekacce evamāhaṁsu:",
  "ud3.9:2.24_0": "“khattavijjāsippaṁ sippānaṁ aggan”ti.",
  "ud3.9:2.25_0": "Ayañcarahi tesaṁ bhikkhūnaṁ antarākathā hoti vippakatā.",
  "ud3.9:3.1_0": "Atha kho bhagavā sāyanhasamayaṁ paṭisallānā vuṭṭhito yena maṇḍalamāḷo tenupasaṅkami;",
  "ud3.9:3.1_1": "upasaṅkamitvā paññatte āsane nisīdi.",
  "ud3.9:3.2_0": "Nisajja kho bhagavā bhikkhū āmantesi:",
  "ud3.9:3.3_0": "“kāya nuttha, bhikkhave, etarahi kathāya sannisinnā, kā ca pana vo antarākathā vippakatā”ti?",
  "ud3.9:4.1_0": "“Idha, bhante, amhākaṁ pacchābhattaṁ piṇḍapātapaṭikkantānaṁ maṇḍalamāḷe sannisinnānaṁ ayamantarākathā udapādi:",
  "ud3.9:4.2_0": "‘ko nu kho, āvuso, sippaṁ jānāti?",
  "ud3.9:4.3_0": "Ko kiṁ sippaṁ sikkhi?",
  "ud3.9:4.4_0": "Kataraṁ sippaṁ sippānaṁ aggan’ti?",
  "ud3.9:5.1_0": "Tatthekacce evamāhaṁsu:",
  "ud3.9:5.2_0": "‘hatthisippaṁ sippānaṁ aggan’ti.",
  "ud3.9:5.3_0": "Ekacce evamāhaṁsu:",
  "ud3.9:5.4_0": "‘assasippaṁ sippānaṁ aggan’ti.",
  "ud3.9:5.5_0": "Ekacce evamāhaṁsu:",
  "ud3.9:5.6_0": "‘rathasippaṁ sippānaṁ aggan’ti.",
  "ud3.9:5.7_0": "Ekacce evamāhaṁsu:",
  "ud3.9:5.8_0": "‘dhanusippaṁ sippānaṁ aggan’ti.",
  "ud3.9:5.9_0": "Ekacce evamāhaṁsu:",
  "ud3.9:5.10_0": "‘tharusippaṁ sippānaṁ aggan’ti.",
  "ud3.9:5.11_0": "Ekacce evamāhaṁsu:",
  "ud3.9:5.12_0": "‘muddāsippaṁ sippānaṁ aggan’ti.",
  "ud3.9:5.13_0": "Ekacce evamāhaṁsu:",
  "ud3.9:5.14_0": "‘gaṇanāsippaṁ sippānaṁ aggan’ti.",
  "ud3.9:5.15_0": "Ekacce evamāhaṁsu:",
  "ud3.9:5.16_0": "‘saṅkhānasippaṁ sippānaṁ aggan’ti.",
  "ud3.9:5.17_0": "Ekacce evamāhaṁsu:",
  "ud3.9:5.18_0": "‘lekhāsippaṁ sippānaṁ aggan’ti.",
  "ud3.9:5.19_0": "Ekacce evamāhaṁsu:",
  "ud3.9:5.20_0": "‘kāveyyasippaṁ sippānaṁ aggan’ti.",
  "ud3.9:5.21_0": "Ekacce evamāhaṁsu:",
  "ud3.9:5.22_0": "‘lokāyatasippaṁ sippānaṁ aggan’ti.",
  "ud3.9:5.23_0": "Ekacce evamāhaṁsu:",
  "ud3.9:5.24_0": "‘khattavijjāsippaṁ sippānaṁ aggan’ti.",
  "ud3.9:5.25_0": "Ayaṁ kho no, bhante, antarākathā hoti vippakatā, atha bhagavā anuppatto”ti.",
  "ud3.9:6.1_0": "“Na khvetaṁ, bhikkhave, tumhākaṁ patirūpaṁ kulaputtānaṁ saddhā agārasmā anagāriyaṁ pabbajitānaṁ yaṁ tumhe evarūpiṁ kathaṁ katheyyātha.",
  "ud3.9:6.2_0": "Sannipatitānaṁ vo, bhikkhave, dvayaṁ karaṇīyaṁ—",
  "ud3.9:6.3_0": "dhammī vā kathā ariyo vā tuṇhībhāvo”ti.",
  "ud3.9:7.1_0": "Atha kho bhagavā etamatthaṁ viditvā tāyaṁ velāyaṁ imaṁ udānaṁ udānesi:",
  "ud3.9:8.1_0": "“Asippajīvī lahu atthakāmo,",
  "ud3.9:8.2_0": "Yatindriyo sabbadhi vippamutto;",
  "ud3.9:8.3_0": "Anokasārī amamo nirāso,",
  "ud3.9:8.4_0": "Hitvā mānaṁ ekacaro sa bhikkhū”ti.",
  "ud3.9:8.5_0": "Navamaṁ."
}