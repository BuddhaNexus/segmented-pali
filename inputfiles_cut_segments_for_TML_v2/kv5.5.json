{
  "kv5.5:0.1_0": "Kathāvatthu",
  "kv5.5:0.2_0": "Mahāpaṇṇāsaka",
  "kv5.5:0.3_0": "Pañcamavagga",
  "kv5.5:0.4_0": "Paṭisambhidākathā",
  "kv5.5:1.1_0": "Sabbaṁ ñāṇaṁ paṭisambhidāti?",
  "kv5.5:1.2_0": "Āmantā.",
  "kv5.5:1.3_0": "Sammutiñāṇaṁ paṭisambhidāti?",
  "kv5.5:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv5.5:1.5_0": "sammutiñāṇaṁ paṭisambhidāti?",
  "kv5.5:1.6_0": "Āmantā.",
  "kv5.5:1.7_0": "Ye keci sammutiṁ jānanti, sabbe te paṭisambhidāpattāti?",
  "kv5.5:1.8_0": "Na hevaṁ vattabbe …pe…",
  "kv5.5:1.9_0": "sabbaṁ ñāṇaṁ paṭisambhidāti?",
  "kv5.5:1.10_0": "Āmantā.",
  "kv5.5:1.11_0": "Cetopariyāye ñāṇaṁ paṭisambhidāti?",
  "kv5.5:1.12_0": "Na hevaṁ vattabbe …pe…",
  "kv5.5:1.13_0": "cetopariyāye ñāṇaṁ paṭisambhidāti?",
  "kv5.5:1.14_0": "Āmantā.",
  "kv5.5:1.15_0": "Ye keci paracittaṁ jānanti, sabbe te paṭisambhidāpattāti?",
  "kv5.5:1.16_0": "Na hevaṁ vattabbe …pe….",
  "kv5.5:2.1_0": "Sabbaṁ ñāṇaṁ paṭisambhidāti?",
  "kv5.5:2.2_0": "Āmantā.",
  "kv5.5:2.3_0": "Sabbā paññā paṭisambhidāti?",
  "kv5.5:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv5.5:2.5_0": "sabbā paññā paṭisambhidāti?",
  "kv5.5:2.6_0": "Āmantā.",
  "kv5.5:2.7_0": "Pathavīkasiṇaṁ samāpattiṁ samāpannassa atthi paññā, sā paññā paṭisambhidāti?",
  "kv5.5:2.8_0": "Na hevaṁ vattabbe …pe…",
  "kv5.5:2.9_0": "āpokasiṇaṁ …pe…",
  "kv5.5:2.10_0": "tejokasiṇaṁ …pe…",
  "kv5.5:2.11_0": "vāyokasiṇaṁ …pe…",
  "kv5.5:2.12_0": "nīlakasiṇaṁ …pe…",
  "kv5.5:2.13_0": "pītakasiṇaṁ …pe…",
  "kv5.5:2.14_0": "lohitakasiṇaṁ …pe…",
  "kv5.5:2.15_0": "odātakasiṇaṁ …pe…",
  "kv5.5:2.16_0": "ākāsānañcāyatanaṁ …pe…",
  "kv5.5:2.17_0": "viññāṇañcāyatanaṁ …pe…",
  "kv5.5:2.18_0": "ākiñcaññāyatanaṁ …pe…",
  "kv5.5:2.19_0": "nevasaññānāsaññāyatanaṁ samāpannassa …pe…",
  "kv5.5:2.20_0": "dānaṁ dadantassa …pe…",
  "kv5.5:2.21_0": "cīvaraṁ dadantassa …pe…",
  "kv5.5:2.22_0": "piṇḍapātaṁ dadantassa …pe…",
  "kv5.5:2.23_0": "senāsanaṁ dadantassa …pe…",
  "kv5.5:2.24_0": "gilānapaccayabhesajjaparikkhāraṁ dadantassa atthi paññā, sā paññā paṭisambhidāti?",
  "kv5.5:2.25_0": "Na hevaṁ vattabbe …pe….",
  "kv5.5:3.1_0": "Na vattabbaṁ—",
  "kv5.5:3.2_0": "“sabbaṁ ñāṇaṁ paṭisambhidā”ti?",
  "kv5.5:3.3_0": "Āmantā.",
  "kv5.5:3.4_0": "Atthi lokuttarā paññā, sā paññā na paṭisambhidāti?",
  "kv5.5:3.5_0": "Na hevaṁ vattabbe …pe…",
  "kv5.5:3.6_0": "tena hi sabbaṁ ñāṇaṁ paṭisambhidāti.",
  "kv5.5:3.7_0": "Paṭisambhidākathā niṭṭhitā."
}