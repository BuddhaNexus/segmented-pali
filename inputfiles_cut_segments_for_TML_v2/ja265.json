{
  "ja265:0.1_0": "Jātaka",
  "ja265:0.2_0": "Tikanipāta",
  "ja265:0.3_0": "Padumavagga",
  "ja265:0.4_0": "5. Khurappajātaka",
  "ja265:1.1_0": "“Disvā khurappe dhanuveganunne,",
  "ja265:1.2_0": "Khagge gahīte tikhiṇe teladhote;",
  "ja265:1.3_0": "Tasmiṁ bhayasmiṁ maraṇe viyūḷhe,",
  "ja265:1.4_0": "Kasmā nu te nāhu chambhitattaṁ”.",
  "ja265:2.1_0": "“Disvā khurappe dhanuveganunne,",
  "ja265:2.2_0": "Khagge gahīte tikhiṇe teladhote;",
  "ja265:2.3_0": "Tasmiṁ bhayasmiṁ maraṇe viyūḷhe,",
  "ja265:2.4_0": "Vedaṁ alatthaṁ vipulaṁ uḷāraṁ.",
  "ja265:3.1_0": "So vedajāto ajjhabhaviṁ amitte,",
  "ja265:3.2_0": "Pubbeva me jīvitamāsi cattaṁ;",
  "ja265:3.3_0": "Na hi jīvite ālayaṁ kubbamāno,",
  "ja265:3.4_0": "Sūro kayirā sūrakiccaṁ kadācī”ti.",
  "ja265:4.1_0": "Khurappajātakaṁ pañcamaṁ."
}