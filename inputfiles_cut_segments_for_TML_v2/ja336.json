{
  "ja336:0.1_0": "Jātaka",
  "ja336:0.2_0": "Catukkanipāta",
  "ja336:0.3_0": "Kokilavagga",
  "ja336:0.4_0": "6. Brahāchattajātaka",
  "ja336:1.1_0": "“Tiṇaṁ tiṇanti lapasi,",
  "ja336:1.2_0": "ko nu te tiṇamāhari;",
  "ja336:1.3_0": "Kiṁ nu te tiṇakiccatthi,",
  "ja336:1.4_0": "tiṇameva pabhāsasi”.",
  "ja336:2.1_0": "“Idhāgamā brahmacārī,",
  "ja336:2.2_0": "brahā chatto bahussuto;",
  "ja336:2.3_0": "So me sabbaṁ samādāya,",
  "ja336:2.4_0": "tiṇaṁ nikkhippa gacchati”.",
  "ja336:3.1_0": "“Evetaṁ hoti kattabbaṁ,",
  "ja336:3.2_0": "appena bahumicchatā;",
  "ja336:3.3_0": "Sabbaṁ sakassa ādānaṁ,",
  "ja336:3.4_0": "anādānaṁ tiṇassa ca”.",
  "ja336:3.5_0": "(…)",
  "ja336:4.1_0": "“Sīlavanto na kubbanti,",
  "ja336:4.2_0": "bālo sīlāni kubbati;",
  "ja336:4.3_0": "Aniccasīlaṁ dussīlyaṁ,",
  "ja336:4.4_0": "kiṁ paṇḍiccaṁ karissatī”ti.",
  "ja336:5.1_0": "Brahāchattajātakaṁ chaṭṭhaṁ."
}