{
  "tha-ap514:0.1_0": "Therāpadāna",
  "tha-ap514:0.2_0": "Phaladāyakavagga",
  "tha-ap514:0.3_0": "2 Kapitthaphaladāyakattheraapadāna",
  "tha-ap514:1.1_0": "“Suvaṇṇavaṇṇaṁ sambuddhaṁ,",
  "tha-ap514:1.2_0": "Āhutīnaṁ paṭiggahaṁ;",
  "tha-ap514:1.3_0": "Rathiyaṁ paṭipajjantaṁ,",
  "tha-ap514:1.4_0": "Kapitthaṁ adadiṁ phalaṁ.",
  "tha-ap514:2.1_0": "Ekanavutito kappe,",
  "tha-ap514:2.2_0": "yaṁ phalaṁ adadiṁ tadā;",
  "tha-ap514:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap514:2.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap514:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap514:3.2_0": "…pe…",
  "tha-ap514:3.3_0": "viharāmi anāsavo.",
  "tha-ap514:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap514:4.2_0": "…pe…",
  "tha-ap514:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap514:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap514:5.2_0": "…pe…",
  "tha-ap514:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap514:6.1_0": "Itthaṁ sudaṁ āyasmā kapitthaphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap514:7.1_0": "Kapitthaphaladāyakattherassāpadānaṁ dutiyaṁ."
}