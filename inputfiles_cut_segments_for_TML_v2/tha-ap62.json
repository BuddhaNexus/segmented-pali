{
  "tha-ap62:0.1_0": "Therāpadāna",
  "tha-ap62:0.2_0": "Bījanivagga",
  "tha-ap62:0.3_0": "10. Asanabodhiyattheraapadāna",
  "tha-ap62:1.1_0": "“Jātiyā sattavassohaṁ,",
  "tha-ap62:1.2_0": "addasaṁ lokanāyakaṁ;",
  "tha-ap62:1.3_0": "Pasannacitto sumano,",
  "tha-ap62:1.4_0": "upagacchiṁ naruttamaṁ.",
  "tha-ap62:2.1_0": "Tissassāhaṁ bhagavato,",
  "tha-ap62:2.2_0": "lokajeṭṭhassa tādino;",
  "tha-ap62:2.3_0": "Haṭṭho haṭṭhena cittena,",
  "tha-ap62:2.4_0": "ropayiṁ bodhimuttamaṁ.",
  "tha-ap62:3.1_0": "Asano nāmadheyyena,",
  "tha-ap62:3.2_0": "dharaṇīruhapādapo;",
  "tha-ap62:3.3_0": "Pañcavasse paricariṁ,",
  "tha-ap62:3.4_0": "asanaṁ bodhimuttamaṁ.",
  "tha-ap62:4.1_0": "Pupphitaṁ pādapaṁ disvā,",
  "tha-ap62:4.2_0": "abbhutaṁ lomahaṁsanaṁ;",
  "tha-ap62:4.3_0": "Sakaṁ kammaṁ pakittento,",
  "tha-ap62:4.4_0": "buddhaseṭṭhaṁ upāgamiṁ.",
  "tha-ap62:5.1_0": "Tisso tadā so sambuddho,",
  "tha-ap62:5.2_0": "sayambhū aggapuggalo;",
  "tha-ap62:5.3_0": "Bhikkhusaṅghe nisīditvā,",
  "tha-ap62:5.4_0": "imā gāthā abhāsatha.",
  "tha-ap62:6.1_0": "‘Yenāyaṁ ropitā bodhi,",
  "tha-ap62:6.2_0": "buddhapūjā ca sakkatā;",
  "tha-ap62:6.3_0": "Tamahaṁ kittayissāmi,",
  "tha-ap62:6.4_0": "suṇātha mama bhāsato.",
  "tha-ap62:7.1_0": "Tiṁsakappāni devesu,",
  "tha-ap62:7.2_0": "devarajjaṁ karissati;",
  "tha-ap62:7.3_0": "Catusaṭṭhi cakkhattuṁ so,",
  "tha-ap62:7.4_0": "cakkavattī bhavissati.",
  "tha-ap62:8.1_0": "Tusitā hi cavitvāna,",
  "tha-ap62:8.2_0": "sukkamūlena codito;",
  "tha-ap62:8.3_0": "Dve sampattī anubhotvā,",
  "tha-ap62:8.4_0": "manussatte ramissati.",
  "tha-ap62:9.1_0": "Padhānapahitatto so,",
  "tha-ap62:9.2_0": "upasanto nirūpadhi;",
  "tha-ap62:9.3_0": "Sabbāsave pariññāya,",
  "tha-ap62:9.4_0": "nibbāyissatināsavo’.",
  "tha-ap62:10.1_0": "Vivekamanuyuttohaṁ,",
  "tha-ap62:10.2_0": "upasanto nirūpadhi;",
  "tha-ap62:10.3_0": "Nāgova bandhanaṁ chetvā,",
  "tha-ap62:10.4_0": "viharāmi anāsavo.",
  "tha-ap62:11.1_0": "Dvenavute ito kappe,",
  "tha-ap62:11.2_0": "bodhiṁ ropesahaṁ tadā;",
  "tha-ap62:11.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap62:11.4_0": "bodhiropassidaṁ phalaṁ.",
  "tha-ap62:12.1_0": "Catusattatito kappe,",
  "tha-ap62:12.2_0": "daṇḍasenoti vissuto;",
  "tha-ap62:12.3_0": "Sattaratanasampanno,",
  "tha-ap62:12.4_0": "cakkavattī tadā ahuṁ.",
  "tha-ap62:13.1_0": "Tesattatimhito kappe,",
  "tha-ap62:13.2_0": "sattāhesuṁ mahīpatī;",
  "tha-ap62:13.3_0": "Samantanemināmena,",
  "tha-ap62:13.4_0": "rājāno cakkavattino.",
  "tha-ap62:14.1_0": "Paṇṇavīsatito kappe,",
  "tha-ap62:14.2_0": "puṇṇako nāma khattiyo;",
  "tha-ap62:14.3_0": "Sattaratanasampanno,",
  "tha-ap62:14.4_0": "cakkavattī mahabbalo.",
  "tha-ap62:15.1_0": "Paṭisambhidā catasso,",
  "tha-ap62:15.2_0": "…pe…",
  "tha-ap62:15.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap62:16.1_0": "Itthaṁ sudaṁ āyasmā asanabodhiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap62:17.1_0": "Asanabodhiyattherassāpadānaṁ dasamaṁ.",
  "tha-ap62:18.1_0": "Bījanivaggo chaṭṭho.",
  "tha-ap62:19.1_0": "Tassuddānaṁ",
  "tha-ap62:20.1_0": "Bījanī sataraṁsī ca,",
  "tha-ap62:20.2_0": "sayanodakivāhiyo;",
  "tha-ap62:20.3_0": "Parivāro padīpañca,",
  "tha-ap62:20.4_0": "dhajo padumapūjako;",
  "tha-ap62:20.5_0": "Bodhi ca dasamo vutto,",
  "tha-ap62:20.6_0": "gāthā dvenavuti tathā."
}