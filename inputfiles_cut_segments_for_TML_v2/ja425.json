{
  "ja425:0.1_0": "Jātaka",
  "ja425:0.2_0": "Aṭṭhakanipāta",
  "ja425:0.3_0": "Kaccānivagga",
  "ja425:0.4_0": "9. Aṭṭhānajātaka",
  "ja425:1.1_0": "“Gaṅgā kumudinī santā,",
  "ja425:1.2_0": "saṅkhavaṇṇā ca kokilā;",
  "ja425:1.3_0": "Jambu tālaphalaṁ dajjā,",
  "ja425:1.4_0": "atha nūna tadā siyā.",
  "ja425:2.1_0": "Yadā kacchapalomānaṁ,",
  "ja425:2.2_0": "pāvāro tividho siyā;",
  "ja425:2.3_0": "Hemantikaṁ pāvuraṇaṁ,",
  "ja425:2.4_0": "atha nūna tadā siyā.",
  "ja425:3.1_0": "Yadā makasapādānaṁ,",
  "ja425:3.2_0": "aṭṭālo sukato siyā;",
  "ja425:3.3_0": "Daḷho ca avikampī ca,",
  "ja425:3.4_0": "atha nūna tadā siyā.",
  "ja425:4.1_0": "Yadā sasavisāṇānaṁ,",
  "ja425:4.2_0": "nisseṇī sukatā siyā;",
  "ja425:4.3_0": "Saggassārohaṇatthāya,",
  "ja425:4.4_0": "atha nūna tadā siyā.",
  "ja425:5.1_0": "Yadā nisseṇimāruyha,",
  "ja425:5.2_0": "candaṁ khādeyyu mūsikā;",
  "ja425:5.3_0": "Rāhuñca paripāteyyuṁ,",
  "ja425:5.4_0": "atha nūna tadā siyā.",
  "ja425:6.1_0": "Yadā surāghaṭaṁ pitvā,",
  "ja425:6.2_0": "makkhikā gaṇacāriṇī;",
  "ja425:6.3_0": "Aṅgāre vāsaṁ kappeyyuṁ,",
  "ja425:6.4_0": "atha nūna tadā siyā.",
  "ja425:7.1_0": "Yadā bimboṭṭhasampanno,",
  "ja425:7.2_0": "gadrabho sumukho siyā;",
  "ja425:7.3_0": "Kusalo naccagītassa,",
  "ja425:7.4_0": "atha nūna tadā siyā.",
  "ja425:8.1_0": "Yadā kākā ulūkā ca,",
  "ja425:8.2_0": "mantayeyyuṁ rahogatā;",
  "ja425:8.3_0": "Aññamaññaṁ pihayeyyuṁ,",
  "ja425:8.4_0": "atha nūna tadā siyā.",
  "ja425:9.1_0": "Yadā muḷāla pattānaṁ,",
  "ja425:9.2_0": "chattaṁ thirataraṁ siyā;",
  "ja425:9.3_0": "Vassassa paṭighātāya,",
  "ja425:9.4_0": "atha nūna tadā siyā.",
  "ja425:10.1_0": "Yadā kulako sakuṇo,",
  "ja425:10.2_0": "pabbataṁ gandhamādanaṁ;",
  "ja425:10.3_0": "Tuṇḍenādāya gaccheyya,",
  "ja425:10.4_0": "atha nūna tadā siyā.",
  "ja425:11.1_0": "Yadā sāmuddikaṁ nāvaṁ,",
  "ja425:11.2_0": "sayantaṁ savaṭākaraṁ;",
  "ja425:11.3_0": "Ceṭo ādāya gaccheyya,",
  "ja425:11.4_0": "atha nūna tadā siyā”ti.",
  "ja425:12.1_0": "Aṭṭhānajātakaṁ navamaṁ."
}