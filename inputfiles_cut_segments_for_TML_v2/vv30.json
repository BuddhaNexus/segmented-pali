{
  "vv30:0.1_0": "Vimānavatthu",
  "vv30:0.2_0": "Itthivimāna",
  "vv30:0.3_0": "Pāricchattakavagga",
  "vv30:0.4_0": "2. Ucchudāyikāvimānavatthu",
  "vv30:1.1_0": "“Obhāsayitvā pathaviṁ sadevakaṁ,",
  "vv30:1.2_0": "Atirocasi candimasūriyā viya;",
  "vv30:1.3_0": "Siriyā ca vaṇṇena yasena tejasā,",
  "vv30:1.4_0": "Brahmāva deve tidase sahindake.",
  "vv30:2.1_0": "Pucchāmi taṁ uppalamāladhārinī,",
  "vv30:2.2_0": "Āveḷinī kañcanasannibhattace;",
  "vv30:2.3_0": "Alaṅkate uttamavatthadhārinī,",
  "vv30:2.4_0": "Kā tvaṁ subhe devate vandase mamaṁ.",
  "vv30:3.1_0": "Kiṁ tvaṁ pure kammamakāsi attanā,",
  "vv30:3.2_0": "Manussabhūtā purimāya jātiyā;",
  "vv30:3.3_0": "Dānaṁ suciṇṇaṁ atha sīlasaṁyamaṁ,",
  "vv30:3.4_0": "Kenūpapannā sugatiṁ yasassinī;",
  "vv30:3.5_0": "Devate pucchitācikkha,",
  "vv30:3.6_0": "Kissa kammassidaṁ phalan”ti.",
  "vv30:4.1_0": "“Idāni bhante imameva gāmaṁ,",
  "vv30:4.2_0": "Piṇḍāya amhākaṁ gharaṁ upāgami;",
  "vv30:4.3_0": "Tato te ucchussa adāsi khaṇḍikaṁ,",
  "vv30:4.4_0": "Pasannacittā atulāya pītiyā.",
  "vv30:5.1_0": "Sassu ca pacchā anuyuñjate mamaṁ,",
  "vv30:5.2_0": "Kahaṁ nu ucchuṁ vadhuke avākiri;",
  "vv30:5.3_0": "Na chaḍḍitaṁ no pana khāditaṁ mayā,",
  "vv30:5.4_0": "Santassa bhikkhussa sayaṁ adāsahaṁ.",
  "vv30:6.1_0": "Tuyhaṁ nvidaṁ issariyaṁ atho mama,",
  "vv30:6.2_0": "Itissā sassu paribhāsate mamaṁ;",
  "vv30:6.3_0": "Pīṭhaṁ gahetvā pahāraṁ adāsi me,",
  "vv30:6.4_0": "Tato cutā kālakatāmhi devatā.",
  "vv30:7.1_0": "Tadeva kammaṁ kusalaṁ kataṁ mayā,",
  "vv30:7.2_0": "Sukhañca kammaṁ anubhomi attanā;",
  "vv30:7.3_0": "Devehi saddhiṁ paricārayāmahaṁ,",
  "vv30:7.4_0": "Modāmahaṁ kāmaguṇehi pañcahi.",
  "vv30:8.1_0": "Tadeva kammaṁ kusalaṁ kataṁ mayā,",
  "vv30:8.2_0": "Sukhañca kammaṁ anubhomi attanā;",
  "vv30:8.3_0": "Devindaguttā tidasehi rakkhitā,",
  "vv30:8.4_0": "Samappitā kāmaguṇehi pañcahi.",
  "vv30:9.1_0": "Etādisaṁ puññaphalaṁ anappakaṁ,",
  "vv30:9.2_0": "Mahāvipākā mama ucchudakkhiṇā;",
  "vv30:9.3_0": "Devehi saddhiṁ paricārayāmahaṁ,",
  "vv30:9.4_0": "Modāmahaṁ kāmaguṇehi pañcahi.",
  "vv30:10.1_0": "Etādisaṁ puññaphalaṁ anappakaṁ,",
  "vv30:10.2_0": "Mahājutikā mama ucchudakkhiṇā;",
  "vv30:10.3_0": "Devindaguttā tidasehi rakkhitā,",
  "vv30:10.4_0": "Sahassanettoriva nandane vane.",
  "vv30:11.1_0": "Tuvañca bhante anukampakaṁ viduṁ,",
  "vv30:11.2_0": "Upecca vandiṁ kusalañca pucchisaṁ;",
  "vv30:11.3_0": "Tato te ucchussa adāsi khaṇḍikaṁ,",
  "vv30:11.4_0": "Pasannacittā atulāya pītiyā”ti.",
  "vv30:12.1_0": "Ucchudāyikāvimānaṁ dutiyaṁ."
}