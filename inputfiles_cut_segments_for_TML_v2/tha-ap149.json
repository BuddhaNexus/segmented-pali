{
  "tha-ap149:0.1_0": "Therāpadāna",
  "tha-ap149:0.2_0": "Chattavagga",
  "tha-ap149:0.3_0": "7. Maggadāyakattheraapadāna",
  "tha-ap149:1.1_0": "“Uttaritvāna nadikaṁ,",
  "tha-ap149:1.2_0": "vanaṁ gacchati cakkhumā;",
  "tha-ap149:1.3_0": "Tamaddasāsiṁ sambuddhaṁ,",
  "tha-ap149:1.4_0": "siddhatthaṁ varalakkhaṇaṁ.",
  "tha-ap149:2.1_0": "Kudāla piṭakamādāya,",
  "tha-ap149:2.2_0": "samaṁ katvāna taṁ pathaṁ;",
  "tha-ap149:2.3_0": "Satthāraṁ abhivādetvā,",
  "tha-ap149:2.4_0": "sakaṁ cittaṁ pasādayiṁ.",
  "tha-ap149:3.1_0": "Catunnavutito kappe,",
  "tha-ap149:3.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap149:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap149:3.4_0": "maggadānassidaṁ phalaṁ.",
  "tha-ap149:4.1_0": "Sattapaññāsakappamhi,",
  "tha-ap149:4.2_0": "eko āsiṁ janādhipo;",
  "tha-ap149:4.3_0": "Nāmena suppabuddhoti,",
  "tha-ap149:4.4_0": "nāyako so narissaro.",
  "tha-ap149:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap149:5.2_0": "…pe…",
  "tha-ap149:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap149:6.1_0": "Itthaṁ sudaṁ āyasmā maggadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap149:7.1_0": "Maggadāyakattherassāpadānaṁ sattamaṁ."
}