{
  "ja316:0.1_0": "Jātaka",
  "ja316:0.2_0": "Catukkanipāta",
  "ja316:0.3_0": "Pucimandavagga",
  "ja316:0.4_0": "6. Sasapaṇḍitajātaka",
  "ja316:1.1_0": "“Satta me rohitā macchā,",
  "ja316:1.2_0": "udakā thalamubbhatā;",
  "ja316:1.3_0": "Idaṁ brāhmaṇa me atthi,",
  "ja316:1.4_0": "etaṁ bhutvā vane vasa”.",
  "ja316:2.1_0": "“Dussa me khettapālassa,",
  "ja316:2.2_0": "rattibhattaṁ apābhataṁ;",
  "ja316:2.3_0": "Maṁsasūlā ca dve godhā,",
  "ja316:2.4_0": "ekañca dadhivārakaṁ;",
  "ja316:2.5_0": "Idaṁ brāhmaṇa me atthi,",
  "ja316:2.6_0": "etaṁ bhutvā vane vasa”.",
  "ja316:3.1_0": "“Ambapakkaṁ dakaṁ sītaṁ,",
  "ja316:3.2_0": "Sītacchāyā manoramā;",
  "ja316:3.3_0": "Idaṁ brāhmaṇa me atthi,",
  "ja316:3.4_0": "Etaṁ bhutvā vane vasa”.",
  "ja316:4.1_0": "“Na sasassa tilā atthi,",
  "ja316:4.2_0": "Na muggā napi taṇḍulā;",
  "ja316:4.3_0": "Iminā agginā pakkaṁ,",
  "ja316:4.4_0": "Mamaṁ bhutvā vane vasā”ti.",
  "ja316:5.1_0": "Sasapaṇḍitajātakaṁ chaṭṭhaṁ."
}