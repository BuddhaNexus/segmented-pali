{
  "thi-ap28:0.1_0": "Therīapadāna",
  "thi-ap28:0.2_0": "Kuṇḍalakesīvagga",
  "thi-ap28:0.3_0": "8. Yasodharātherīapadāna",
  "thi-ap28:1.1_0": "Ekasmiṁ samaye ramme,",
  "thi-ap28:1.2_0": "iddhe rājagahe pure;",
  "thi-ap28:1.3_0": "Pabbhāramhi varekamhi,",
  "thi-ap28:1.4_0": "vasante naranāyake.",
  "thi-ap28:2.1_0": "Vasantiyā tamhi nagare,",
  "thi-ap28:2.2_0": "ramme bhikkhunupassaye;",
  "thi-ap28:2.3_0": "Yasodharābhikkhuniyā,",
  "thi-ap28:2.4_0": "evaṁ āsi vitakkitaṁ.",
  "thi-ap28:3.1_0": "“Suddhodano mahārājā,",
  "thi-ap28:3.2_0": "gotamī ca pajāpatī;",
  "thi-ap28:3.3_0": "Abhiññātā mahātherā,",
  "thi-ap28:3.4_0": "theriyo ca mahiddhikā.",
  "thi-ap28:4.1_0": "Santiṁ gatāva āsuṁ te,",
  "thi-ap28:4.2_0": "dīpaccīva nirāsavā;",
  "thi-ap28:4.3_0": "Lokanāthe dharanteva,",
  "thi-ap28:4.4_0": "ahampi ca sivaṁ padaṁ.",
  "thi-ap28:5.1_0": "Gamissāmī”ti cintetvā,",
  "thi-ap28:5.2_0": "passantī āyumattano;",
  "thi-ap28:5.3_0": "Passitvā āyusaṅkhāraṁ,",
  "thi-ap28:5.4_0": "tadaheva khayaṁ gataṁ.",
  "thi-ap28:6.1_0": "Pattacīvaramādāya,",
  "thi-ap28:6.2_0": "nikkhamitvā sakassamā;",
  "thi-ap28:6.3_0": "Purakkhatā bhikkhunībhi,",
  "thi-ap28:6.4_0": "satehi sahassehi sā.",
  "thi-ap28:7.1_0": "Mahiddhikā mahāpaññā,",
  "thi-ap28:7.2_0": "sambuddhaṁ upasaṅkami;",
  "thi-ap28:7.3_0": "Sambuddhaṁ abhivādetvā,",
  "thi-ap28:7.4_0": "satthuno cakkalakkhaṇe;",
  "thi-ap28:7.5_0": "Nisinnā ekamantamhi,",
  "thi-ap28:7.6_0": "idaṁ vacanamabravi.",
  "thi-ap28:8.1_0": "“Aṭṭhasattativassāhaṁ,",
  "thi-ap28:8.2_0": "pacchimo vattate vayo;",
  "thi-ap28:8.3_0": "Pabbhāramhi anuppattā,",
  "thi-ap28:8.4_0": "ārocemi mahāmuni.",
  "thi-ap28:9.1_0": "Paripakko vayo mayhaṁ,",
  "thi-ap28:9.2_0": "parittaṁ mama jīvitaṁ;",
  "thi-ap28:9.3_0": "Pahāya vo gamissāmi,",
  "thi-ap28:9.4_0": "kataṁ me saraṇamattano.",
  "thi-ap28:10.1_0": "Vayamhi pacchime kāle,",
  "thi-ap28:10.2_0": "maraṇaṁ uparuddhati;",
  "thi-ap28:10.3_0": "Ajjarattiṁ mahāvīra,",
  "thi-ap28:10.4_0": "pāpuṇissāmi nibbutiṁ.",
  "thi-ap28:11.1_0": "Natthi jāti jarā byādhi,",
  "thi-ap28:11.2_0": "maraṇañca mahāmune;",
  "thi-ap28:11.3_0": "Ajarāmaraṇaṁ puraṁ,",
  "thi-ap28:11.4_0": "gamissāmi asaṅkhataṁ.",
  "thi-ap28:12.1_0": "Yāvatā parisā nāma,",
  "thi-ap28:12.2_0": "samupāsanti satthuno;",
  "thi-ap28:12.3_0": "Aparādhamajānantī,",
  "thi-ap28:12.4_0": "khamantaṁ sammukhā mune.",
  "thi-ap28:13.1_0": "Saṁsaritvā ca saṁsāre,",
  "thi-ap28:13.2_0": "khalitañce mamaṁ tayi;",
  "thi-ap28:13.3_0": "Ārocemi mahāvīra,",
  "thi-ap28:13.4_0": "aparādhaṁ khamassu me”.",
  "thi-ap28:14.1_0": "Sutvāna vacanaṁ tassā,",
  "thi-ap28:14.2_0": "munindo idamabravi;",
  "thi-ap28:14.3_0": "“Kimuttaraṁ te vakkhāmi,",
  "thi-ap28:14.4_0": "nibbānāya vajantiyā.",
  "thi-ap28:15.1_0": "Iddhiñcāpi nidassehi,",
  "thi-ap28:15.2_0": "mama sāsanakārike;",
  "thi-ap28:15.3_0": "Parisānañca sabbāsaṁ,",
  "thi-ap28:15.4_0": "kaṅkhaṁ chindassu yāvatā”.",
  "thi-ap28:16.1_0": "Sutvā taṁ munino vācaṁ,",
  "thi-ap28:16.2_0": "bhikkhunī sā yasodharā;",
  "thi-ap28:16.3_0": "Vanditvā munirājaṁ taṁ,",
  "thi-ap28:16.4_0": "idaṁ vacanamabravi.",
  "thi-ap28:17.1_0": "“Yasodharā ahaṁ vīra,",
  "thi-ap28:17.2_0": "agāre te pajāpati;",
  "thi-ap28:17.3_0": "Sākiyamhi kule jātā,",
  "thi-ap28:17.4_0": "itthiaṅge patiṭṭhitā.",
  "thi-ap28:18.1_0": "Thīnaṁ satasahassānaṁ,",
  "thi-ap28:18.2_0": "navutīnaṁ chaduttari;",
  "thi-ap28:18.3_0": "Agāre te ahaṁ vīra,",
  "thi-ap28:18.4_0": "pāmokkhā sabbā issarā.",
  "thi-ap28:19.1_0": "Rūpācāraguṇūpetā,",
  "thi-ap28:19.2_0": "yobbanaṭṭhā piyaṁvadā;",
  "thi-ap28:19.3_0": "Sabbā maṁ apacāyanti,",
  "thi-ap28:19.4_0": "devatā viya mānusā.",
  "thi-ap28:20.1_0": "Kaññāsatasahassapamukhā,",
  "thi-ap28:20.2_0": "Sakyaputtanivesane;",
  "thi-ap28:20.3_0": "Samānasukhadukkhatā,",
  "thi-ap28:20.4_0": "Devatā viya nandane.",
  "thi-ap28:21.1_0": "Kāmadhātumatikkamma,",
  "thi-ap28:21.2_0": "saṇṭhitā rūpadhātuyā;",
  "thi-ap28:21.3_0": "Rūpena sadisā natthi,",
  "thi-ap28:21.4_0": "ṭhapetvā lokanāyakaṁ”.",
  "thi-ap28:22.1_0": "Sambuddhaṁ abhivādetvā,",
  "thi-ap28:22.2_0": "iddhiṁ dassesi satthuno;",
  "thi-ap28:22.3_0": "Nekā nānāvidhākārā,",
  "thi-ap28:22.4_0": "mahāiddhīpi dassayī.",
  "thi-ap28:23.1_0": "Cakkavāḷasamaṁ kāyaṁ,",
  "thi-ap28:23.2_0": "sīsaṁ uttarato kuru;",
  "thi-ap28:23.3_0": "Ubho pakkhā duve dīpā,",
  "thi-ap28:23.4_0": "jambudīpaṁ sarīrato.",
  "thi-ap28:24.1_0": "Dakkhiṇañca saraṁ piñchaṁ,",
  "thi-ap28:24.2_0": "nānāsākhā tu pattakā;",
  "thi-ap28:24.3_0": "Candañca sūriyañcakkhi,",
  "thi-ap28:24.4_0": "merupabbatato sikhaṁ.",
  "thi-ap28:25.1_0": "Cakkavāḷagiriṁ tuṇḍaṁ,",
  "thi-ap28:25.2_0": "jamburukkhaṁ samūlakaṁ;",
  "thi-ap28:25.3_0": "Bījamānā upāgantvā,",
  "thi-ap28:25.4_0": "vandantī lokanāyakaṁ.",
  "thi-ap28:26.1_0": "Hatthivaṇṇaṁ tathevassaṁ,",
  "thi-ap28:26.2_0": "pabbataṁ jaladhiṁ tathā;",
  "thi-ap28:26.3_0": "Candimaṁ sūriyaṁ meruṁ,",
  "thi-ap28:26.4_0": "sakkavaṇṇañca dassayi.",
  "thi-ap28:27.1_0": "“Yasodharā ahaṁ vīra,",
  "thi-ap28:27.2_0": "pāde vandāmi cakkhuma”;",
  "thi-ap28:27.3_0": "Sahassalokadhātūnaṁ,",
  "thi-ap28:27.4_0": "phullapadmena chādayi.",
  "thi-ap28:28.1_0": "Brahmavaṇṇañca māpetvā,",
  "thi-ap28:28.2_0": "dhammaṁ desesi suññataṁ;",
  "thi-ap28:28.3_0": "“Yasodharā ahaṁ vīra,",
  "thi-ap28:28.4_0": "pāde vandāmi cakkhuma.",
  "thi-ap28:29.1_0": "Iddhīsu ca vasī homi,",
  "thi-ap28:29.2_0": "dibbāya sotadhātuyā;",
  "thi-ap28:29.3_0": "Cetopariyañāṇassa,",
  "thi-ap28:29.4_0": "vasī homi mahāmuni.",
  "thi-ap28:30.1_0": "Pubbenivāsaṁ jānāmi,",
  "thi-ap28:30.2_0": "dibbacakkhu visodhitaṁ;",
  "thi-ap28:30.3_0": "Sabbāsavā parikkhīṇā,",
  "thi-ap28:30.4_0": "natthi dāni punabbhavo.",
  "thi-ap28:31.1_0": "Atthadhammaniruttīsu,",
  "thi-ap28:31.2_0": "paṭibhāne tatheva ca;",
  "thi-ap28:31.3_0": "Ñāṇaṁ mayhaṁ mahāvīra,",
  "thi-ap28:31.4_0": "uppannaṁ tava santike.",
  "thi-ap28:32.1_0": "Pubbānaṁ lokanāthānaṁ,",
  "thi-ap28:32.2_0": "saṅgamaṁ te nidassitaṁ;",
  "thi-ap28:32.3_0": "Adhikāraṁ bahuṁ mayhaṁ,",
  "thi-ap28:32.4_0": "tuyhatthāya mahāmune.",
  "thi-ap28:33.1_0": "Yaṁ mayhaṁ pūritaṁ kammaṁ,",
  "thi-ap28:33.2_0": "Kusalaṁ sarase mune;",
  "thi-ap28:33.3_0": "Tuyhatthāya mahāvīra,",
  "thi-ap28:33.4_0": "Puññaṁ upacitaṁ mayā.",
  "thi-ap28:34.1_0": "Abhabbaṭṭhāne vajjetvā,",
  "thi-ap28:34.2_0": "vārayitvā anācaraṁ;",
  "thi-ap28:34.3_0": "Tuyhatthāya mahāvīra,",
  "thi-ap28:34.4_0": "sañcattaṁ jīvitaṁ mayā.",
  "thi-ap28:35.1_0": "Nekakoṭisahassāni,",
  "thi-ap28:35.2_0": "bhariyatthāyadāsi maṁ;",
  "thi-ap28:35.3_0": "Na tattha vimanā homi,",
  "thi-ap28:35.4_0": "tuyhatthāya mahāmuni.",
  "thi-ap28:36.1_0": "Nekakoṭisahassāni,",
  "thi-ap28:36.2_0": "upakārāyadāsi maṁ;",
  "thi-ap28:36.3_0": "Na tattha vimanā homi,",
  "thi-ap28:36.4_0": "tuyhatthāya mahāmuni.",
  "thi-ap28:37.1_0": "Nekakoṭisahassāni,",
  "thi-ap28:37.2_0": "bhojanatthāyadāsi maṁ;",
  "thi-ap28:37.3_0": "Na tattha vimanā homi,",
  "thi-ap28:37.4_0": "tuyhatthāya mahāmuni.",
  "thi-ap28:38.1_0": "Nekakoṭisahassāni,",
  "thi-ap28:38.2_0": "jīvitāni pariccajiṁ;",
  "thi-ap28:38.3_0": "Bhayamokkhaṁ karissanti,",
  "thi-ap28:38.4_0": "dadāmi mama jīvitaṁ.",
  "thi-ap28:39.1_0": "Aṅgagate alaṅkāre,",
  "thi-ap28:39.2_0": "vatthe nānāvidhe bahū;",
  "thi-ap28:39.3_0": "Itthimaṇḍe na gūhāmi,",
  "thi-ap28:39.4_0": "tuyhatthāya mahāmuni.",
  "thi-ap28:40.1_0": "Dhanadhaññapariccāgaṁ,",
  "thi-ap28:40.2_0": "gāmāni nigamāni ca;",
  "thi-ap28:40.3_0": "Khettaṁ puttā ca dhītā ca,",
  "thi-ap28:40.4_0": "pariccattā mahāmuni.",
  "thi-ap28:41.1_0": "Hatthī assā gavā cāpi,",
  "thi-ap28:41.2_0": "dāsiyo paricārikā;",
  "thi-ap28:41.3_0": "Tuyhatthāya mahāvīra,",
  "thi-ap28:41.4_0": "pariccattā asaṅkhiyā.",
  "thi-ap28:42.1_0": "Yaṁ mayhaṁ paṭimantesi,",
  "thi-ap28:42.2_0": "dānaṁ dassāmi yācake;",
  "thi-ap28:42.3_0": "Vimanaṁ me na passāmi,",
  "thi-ap28:42.4_0": "dadato dānamuttamaṁ.",
  "thi-ap28:43.1_0": "Nānāvidhaṁ bahuṁ dukkhaṁ,",
  "thi-ap28:43.2_0": "saṁsāre ca bahubbidhe;",
  "thi-ap28:43.3_0": "Tuyhatthāya mahāvīra,",
  "thi-ap28:43.4_0": "anubhuttaṁ asaṅkhiyaṁ.",
  "thi-ap28:44.1_0": "Sukhappattānumodāmi,",
  "thi-ap28:44.2_0": "na ca dukkhesu dummanā;",
  "thi-ap28:44.3_0": "Sabbattha tulitā homi,",
  "thi-ap28:44.4_0": "tuyhatthāya mahāmuni.",
  "thi-ap28:45.1_0": "Anumaggena sambuddho,",
  "thi-ap28:45.2_0": "yaṁ dhammaṁ abhinīhari;",
  "thi-ap28:45.3_0": "Anubhotvā sukhaṁ dukkhaṁ,",
  "thi-ap28:45.4_0": "patto bodhiṁ mahāmuni.",
  "thi-ap28:46.1_0": "Brahmadevañca sambuddhaṁ,",
  "thi-ap28:46.2_0": "gotamaṁ lokanāyakaṁ;",
  "thi-ap28:46.3_0": "Aññesaṁ lokanāthānaṁ,",
  "thi-ap28:46.4_0": "saṅgamaṁ te bahuṁ mayā.",
  "thi-ap28:47.1_0": "Adhikāraṁ bahuṁ mayhaṁ,",
  "thi-ap28:47.2_0": "tuyhatthāya mahāmuni;",
  "thi-ap28:47.3_0": "Gavesato buddhadhamme,",
  "thi-ap28:47.4_0": "ahaṁ te paricārikā.",
  "thi-ap28:48.1_0": "Kappe ca satasahasse,",
  "thi-ap28:48.2_0": "caturo ca asaṅkhiye;",
  "thi-ap28:48.3_0": "Dīpaṅkaro mahāvīro,",
  "thi-ap28:48.4_0": "uppajji lokanāyako.",
  "thi-ap28:49.1_0": "Paccantadesavisaye,",
  "thi-ap28:49.2_0": "nimantetvā tathāgataṁ;",
  "thi-ap28:49.3_0": "Tassa āgamanaṁ maggaṁ,",
  "thi-ap28:49.4_0": "sodhenti tuṭṭhamānasā.",
  "thi-ap28:50.1_0": "Tena kālena so āsi,",
  "thi-ap28:50.2_0": "sumedho nāma brāhmaṇo;",
  "thi-ap28:50.3_0": "Maggañca paṭiyādesi,",
  "thi-ap28:50.4_0": "āyato sabbadassino.",
  "thi-ap28:51.1_0": "Tena kālenahaṁ āsiṁ,",
  "thi-ap28:51.2_0": "kaññā brāhmaṇasambhavā;",
  "thi-ap28:51.3_0": "Sumittā nāma nāmena,",
  "thi-ap28:51.4_0": "upagacchiṁ samāgamaṁ.",
  "thi-ap28:52.1_0": "Aṭṭha uppalahatthāni,",
  "thi-ap28:52.2_0": "pūjanatthāya satthuno;",
  "thi-ap28:52.3_0": "Ādāya janasammajjhe,",
  "thi-ap28:52.4_0": "addasaṁ isimuggataṁ.",
  "thi-ap28:53.1_0": "Cirānugataṁ dayitaṁ,",
  "thi-ap28:53.2_0": "atikkantaṁ manoharaṁ;",
  "thi-ap28:53.3_0": "Disvā tadā amaññissaṁ,",
  "thi-ap28:53.4_0": "saphalaṁ jīvitaṁ mama.",
  "thi-ap28:54.1_0": "Parakkamaṁ taṁ saphalaṁ,",
  "thi-ap28:54.2_0": "addasaṁ isino tadā;",
  "thi-ap28:54.3_0": "Pubbakammena sambuddhe,",
  "thi-ap28:54.4_0": "cittañcāpi pasīdi me.",
  "thi-ap28:55.1_0": "Bhiyyo cittaṁ pasādesiṁ,",
  "thi-ap28:55.2_0": "ise uggatamānase;",
  "thi-ap28:55.3_0": "Deyyaṁ aññaṁ na passāmi,",
  "thi-ap28:55.4_0": "demi pupphāni te isi.",
  "thi-ap28:56.1_0": "‘Pañca hatthā tava hontu,",
  "thi-ap28:56.2_0": "tayo hontu mamaṁ ise;",
  "thi-ap28:56.3_0": "Tena saddhiṁ samā hontu,",
  "thi-ap28:56.4_0": "bodhatthāya tavaṁ ise’.",
  "thi-ap28:57.1_0": "Catutthaṁ bhāṇavāraṁ.",
  "thi-ap28:58.1_0": "Isi gahetvā pupphāni,",
  "thi-ap28:58.2_0": "āgacchantaṁ mahāyasaṁ;",
  "thi-ap28:58.3_0": "Pūjesi janasammajjhe,",
  "thi-ap28:58.4_0": "bodhatthāya mahāisi.",
  "thi-ap28:59.1_0": "Passitvā janasammajjhe,",
  "thi-ap28:59.2_0": "dīpaṅkaro mahāmuni;",
  "thi-ap28:59.3_0": "Viyākāsi mahāvīro,",
  "thi-ap28:59.4_0": "isi muggatamānasaṁ.",
  "thi-ap28:60.1_0": "Aparimeyye ito kappe,",
  "thi-ap28:60.2_0": "dīpaṅkaro mahāmuni;",
  "thi-ap28:60.3_0": "Mama kammaṁ viyākāsi,",
  "thi-ap28:60.4_0": "ujubhāvaṁ mahāmuni.",
  "thi-ap28:61.1_0": "‘Samacittā samakammā,",
  "thi-ap28:61.2_0": "samakārī bhavissati;",
  "thi-ap28:61.3_0": "Piyā hessati kammena,",
  "thi-ap28:61.4_0": "tuyhatthāya mahāisi.",
  "thi-ap28:62.1_0": "Sudassanā supiyā ca,",
  "thi-ap28:62.2_0": "manāpā piyavādinī;",
  "thi-ap28:62.3_0": "Tassa dhammesu dāyādā,",
  "thi-ap28:62.4_0": "viharissati iddhikā.",
  "thi-ap28:63.1_0": "Yathāpi bhaṇḍasāmuggaṁ,",
  "thi-ap28:63.2_0": "anurakkhati sāmino;",
  "thi-ap28:63.3_0": "Evaṁ kusaladhammānaṁ,",
  "thi-ap28:63.4_0": "anurakkhissate ayaṁ.",
  "thi-ap28:64.1_0": "Tassa te anukampantī,",
  "thi-ap28:64.2_0": "pūrayissati pāramī;",
  "thi-ap28:64.3_0": "Sīhova pañjaraṁ bhetvā,",
  "thi-ap28:64.4_0": "pāpuṇissati bodhiyaṁ’.",
  "thi-ap28:65.1_0": "Aparimeyye ito kappe,",
  "thi-ap28:65.2_0": "yaṁ maṁ buddho viyākarī;",
  "thi-ap28:65.3_0": "Taṁ vācaṁ anumodentī,",
  "thi-ap28:65.4_0": "evaṅkārī bhaviṁ ahaṁ.",
  "thi-ap28:66.1_0": "Tassa kammassa sukatassa,",
  "thi-ap28:66.2_0": "tattha cittaṁ pasādayiṁ;",
  "thi-ap28:66.3_0": "Devamanussakaṁ yoniṁ,",
  "thi-ap28:66.4_0": "anubhotvā asaṅkhiyaṁ.",
  "thi-ap28:67.1_0": "Sukhadukkhenubhotvāhaṁ,",
  "thi-ap28:67.2_0": "devesu mānusesu ca;",
  "thi-ap28:67.3_0": "Pacchime bhave sampatte,",
  "thi-ap28:67.4_0": "ajāyiṁ sākiye kule.",
  "thi-ap28:68.1_0": "Rūpavatī bhogavatī,",
  "thi-ap28:68.2_0": "yasasīlavatī tato;",
  "thi-ap28:68.3_0": "Sabbaṅgasampadā homi,",
  "thi-ap28:68.4_0": "kulesu abhisakkatā.",
  "thi-ap28:69.1_0": "Lābhaṁ silokaṁ sakkāraṁ,",
  "thi-ap28:69.2_0": "lokadhammasamāgamaṁ;",
  "thi-ap28:69.3_0": "Cittañca dukkhitaṁ natthi,",
  "thi-ap28:69.4_0": "vasāmi akutobhayā.",
  "thi-ap28:70.1_0": "Vuttañhetaṁ bhagavatā,",
  "thi-ap28:70.2_0": "‘rañño antepure tadā;",
  "thi-ap28:70.3_0": "Khattiyānaṁ pure vīra,",
  "thi-ap28:70.4_0": "upakārañca niddisi.",
  "thi-ap28:71.1_0": "Upakārā ca yā nārī,",
  "thi-ap28:71.2_0": "yā ca nārī sukhe dukhe;",
  "thi-ap28:71.3_0": "Atthakkhāyī ca yā nārī,",
  "thi-ap28:71.4_0": "yā ca nārīnukampikā’.",
  "thi-ap28:72.1_0": "Pañcakoṭisatā buddhā,",
  "thi-ap28:72.2_0": "navakoṭisatāni ca;",
  "thi-ap28:72.3_0": "Etesaṁ devadevānaṁ,",
  "thi-ap28:72.4_0": "mahādānaṁ pavattayiṁ.",
  "thi-ap28:73.1_0": "Adhikāraṁ mahā mayhaṁ,",
  "thi-ap28:73.2_0": "dhammarāja suṇohi me;",
  "thi-ap28:73.3_0": "Ekādasakoṭisatā,",
  "thi-ap28:73.4_0": "buddhā dvādasa koṭiyo.",
  "thi-ap28:74.1_0": "Etesaṁ devadevānaṁ,",
  "thi-ap28:74.2_0": "mahādānaṁ pavattayiṁ;",
  "thi-ap28:74.3_0": "Adhikāraṁ mahā mayhaṁ,",
  "thi-ap28:74.4_0": "dhammarāja suṇohi me.",
  "thi-ap28:75.1_0": "Vīsakoṭisatā buddhā,",
  "thi-ap28:75.2_0": "tiṁsakoṭisatāni ca;",
  "thi-ap28:75.3_0": "Etesaṁ devadevānaṁ,",
  "thi-ap28:75.4_0": "mahādānaṁ pavattayiṁ.",
  "thi-ap28:76.1_0": "Adhikāraṁ mahā mayhaṁ,",
  "thi-ap28:76.2_0": "dhammarāja suṇohi me;",
  "thi-ap28:76.3_0": "Cattālīsakoṭisatā,",
  "thi-ap28:76.4_0": "paññāsa koṭisatāni ca.",
  "thi-ap28:77.1_0": "Etesaṁ devadevānaṁ,",
  "thi-ap28:77.2_0": "mahādānaṁ pavattayiṁ;",
  "thi-ap28:77.3_0": "Adhikāraṁ mahā mayhaṁ,",
  "thi-ap28:77.4_0": "dhammarāja suṇohi me.",
  "thi-ap28:78.1_0": "Saṭṭhikoṭisatā buddhā,",
  "thi-ap28:78.2_0": "sattatikoṭisatāni ca;",
  "thi-ap28:78.3_0": "Etesaṁ devadevānaṁ,",
  "thi-ap28:78.4_0": "mahādānaṁ pavattayiṁ.",
  "thi-ap28:79.1_0": "Adhikāraṁ mahā mayhaṁ,",
  "thi-ap28:79.2_0": "dhammarāja suṇohi me;",
  "thi-ap28:79.3_0": "Asītikoṭisatā buddhā,",
  "thi-ap28:79.4_0": "navutikoṭisatāni ca.",
  "thi-ap28:80.1_0": "Etesaṁ devadevānaṁ,",
  "thi-ap28:80.2_0": "mahādānaṁ pavattayiṁ;",
  "thi-ap28:80.3_0": "Adhikāraṁ mahā mayhaṁ,",
  "thi-ap28:80.4_0": "dhammarāja suṇohi me.",
  "thi-ap28:81.1_0": "Koṭisatasahassāni,",
  "thi-ap28:81.2_0": "honti lokagganāyakā;",
  "thi-ap28:81.3_0": "Etesaṁ devadevānaṁ,",
  "thi-ap28:81.4_0": "mahādānaṁ pavattayiṁ.",
  "thi-ap28:82.1_0": "Adhikāraṁ mahā mayhaṁ,",
  "thi-ap28:82.2_0": "dhammarāja suṇohi me;",
  "thi-ap28:82.3_0": "Navakoṭisahassāni,",
  "thi-ap28:82.4_0": "apare lokanāyakā.",
  "thi-ap28:83.1_0": "Etesaṁ devadevānaṁ,",
  "thi-ap28:83.2_0": "mahādānaṁ pavattayiṁ;",
  "thi-ap28:83.3_0": "Adhikāraṁ mahā mayhaṁ,",
  "thi-ap28:83.4_0": "dhammarāja suṇohi me.",
  "thi-ap28:84.1_0": "Koṭisatasahassāni,",
  "thi-ap28:84.2_0": "pañcāsītimahesinaṁ;",
  "thi-ap28:84.3_0": "Pañcāsītikoṭisatā,",
  "thi-ap28:84.4_0": "sattatiṁsā ca koṭiyo.",
  "thi-ap28:85.1_0": "Etesaṁ devadevānaṁ,",
  "thi-ap28:85.2_0": "mahādānaṁ pavattayiṁ;",
  "thi-ap28:85.3_0": "Adhikāraṁ mahā mayhaṁ,",
  "thi-ap28:85.4_0": "dhammarāja suṇohi me.",
  "thi-ap28:86.1_0": "Paccekabuddhā vītarāgā,",
  "thi-ap28:86.2_0": "aṭṭhaṭṭhamakakoṭiyo;",
  "thi-ap28:86.3_0": "Adhikāraṁ mahā mayhaṁ,",
  "thi-ap28:86.4_0": "dhammarāja suṇohi me.",
  "thi-ap28:87.1_0": "Khīṇāsavā vītamalā,",
  "thi-ap28:87.2_0": "asaṅkhiyā buddhasāvakā;",
  "thi-ap28:87.3_0": "Adhikāraṁ mahā mayhaṁ,",
  "thi-ap28:87.4_0": "dhammarāja suṇohi me.",
  "thi-ap28:88.1_0": "Evaṁ dhamme suciṇṇānaṁ,",
  "thi-ap28:88.2_0": "sadā dhammassa cārinaṁ;",
  "thi-ap28:88.3_0": "Dhammacārī sukhaṁ seti,",
  "thi-ap28:88.4_0": "asmiṁ loke paramhi ca.",
  "thi-ap28:89.1_0": "Dhammaṁ care sucaritaṁ,",
  "thi-ap28:89.2_0": "na naṁ duccaritaṁ care;",
  "thi-ap28:89.3_0": "Dhammacārī sukhaṁ seti,",
  "thi-ap28:89.4_0": "asmiṁ loke paramhi ca.",
  "thi-ap28:90.1_0": "Nibbinditvāna saṁsāre,",
  "thi-ap28:90.2_0": "pabbajiṁ anagāriyaṁ;",
  "thi-ap28:90.3_0": "Sahassaparivārena,",
  "thi-ap28:90.4_0": "pabbajitvā akiñcanā.",
  "thi-ap28:91.1_0": "Agāraṁ vijahitvāna,",
  "thi-ap28:91.2_0": "pabbajiṁ anagāriyaṁ;",
  "thi-ap28:91.3_0": "Aḍḍhamāse asampatte,",
  "thi-ap28:91.4_0": "catusaccamapāpuṇiṁ.",
  "thi-ap28:92.1_0": "Cīvaraṁ piṇḍapātañca,",
  "thi-ap28:92.2_0": "paccayaṁ sayanāsanaṁ;",
  "thi-ap28:92.3_0": "Upanenti bahū janā,",
  "thi-ap28:92.4_0": "sāgareyeva ūmiyo.",
  "thi-ap28:93.1_0": "Kilesā jhāpitā mayhaṁ,",
  "thi-ap28:93.2_0": "…pe…",
  "thi-ap28:93.3_0": "viharāmi anāsavā.",
  "thi-ap28:94.1_0": "Svāgataṁ vata me āsi,",
  "thi-ap28:94.2_0": "…pe…",
  "thi-ap28:94.3_0": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap28:95.1_0": "Paṭisambhidā catasso,",
  "thi-ap28:95.2_0": "…pe…",
  "thi-ap28:95.3_0": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap28:96.1_0": "Evaṁ bahuvidhaṁ dukkhaṁ,",
  "thi-ap28:96.2_0": "sampattī ca bahubbidhā;",
  "thi-ap28:96.3_0": "Visuddhibhāvaṁ sampattā,",
  "thi-ap28:96.4_0": "labhāmi sabbasampadā.",
  "thi-ap28:97.1_0": "Yā dadāti sakattānaṁ,",
  "thi-ap28:97.2_0": "puññatthāya mahesino;",
  "thi-ap28:97.3_0": "Sahāyasampadā honti,",
  "thi-ap28:97.4_0": "nibbānapadamasaṅkhataṁ.",
  "thi-ap28:98.1_0": "Parikkhīṇaṁ atītañca,",
  "thi-ap28:98.2_0": "Paccuppannaṁ anāgataṁ;",
  "thi-ap28:98.3_0": "Sabbakammaṁ mamaṁ khīṇaṁ,",
  "thi-ap28:98.4_0": "Pāde vandāmi cakkhuma”.",
  "thi-ap28:99.1_0": "Itthaṁ sudaṁ yasodharā bhikkhunī bhagavato sammukhā imā gāthāyo abhāsitthāti.",
  "thi-ap28:100.1_0": "Yasodharātheriyāpadānaṁ aṭṭhamaṁ."
}