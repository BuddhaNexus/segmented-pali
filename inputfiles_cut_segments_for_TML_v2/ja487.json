{
  "ja487:0.1_0": "Jātaka",
  "ja487:0.2_0": "Pakiṇṇakanipāta",
  "ja487:0.3_0": "Sālikedāravagga",
  "ja487:0.4_0": "4. Uddālakajātaka",
  "ja487:1.1_0": "“Kharājinā jaṭilā paṅkadantā,",
  "ja487:1.2_0": "Dummakkharūpā ye mantaṁ jappanti;",
  "ja487:1.3_0": "Kaccinnu te mānusake payoge,",
  "ja487:1.4_0": "Idaṁ vidū parimuttā apāyā”.",
  "ja487:2.1_0": "“Pāpāni kammāni karetha rāja,",
  "ja487:2.2_0": "Bahussuto ce na careyya dhammaṁ;",
  "ja487:2.3_0": "Sahassavedopi na taṁ paṭicca,",
  "ja487:2.4_0": "Dukkhā pamucce caraṇaṁ apatvā”.",
  "ja487:3.1_0": "“Sahassavedopi na taṁ paṭicca,",
  "ja487:3.2_0": "Dukkhā pamucce caraṇaṁ apatvā;",
  "ja487:3.3_0": "Maññāmi vedā aphalā bhavanti,",
  "ja487:3.4_0": "Sasaṁyamaṁ caraṇaññeva saccaṁ”.",
  "ja487:4.1_0": "“Na heva vedā aphalā bhavanti,",
  "ja487:4.2_0": "Sasaṁyamaṁ caraṇaññeva saccaṁ;",
  "ja487:4.3_0": "Kittiñhi pappoti adhicca vede,",
  "ja487:4.4_0": "Santiṁ puṇāti caraṇena danto”.",
  "ja487:5.1_0": "“Bhaccā mātā pitā bandhū,",
  "ja487:5.2_0": "yena jāto sayeva so;",
  "ja487:5.3_0": "Uddālako ahaṁ bhoto,",
  "ja487:5.4_0": "sottiyākulavaṁsako”.",
  "ja487:6.1_0": "“Kathaṁ bho brāhmaṇo hoti,",
  "ja487:6.2_0": "kathaṁ bhavati kevalī;",
  "ja487:6.3_0": "Kathañca parinibbānaṁ,",
  "ja487:6.4_0": "dhammaṭṭho kinti vuccati”.",
  "ja487:7.1_0": "“Niraṅkatvā aggimādāya brāhmaṇo,",
  "ja487:7.2_0": "Āpo siñcaṁ yajaṁ usseti yūpaṁ;",
  "ja487:7.3_0": "Evaṅkaro brāhmaṇo hoti khemī,",
  "ja487:7.4_0": "Dhamme ṭhitaṁ tena amāpayiṁsu”.",
  "ja487:8.1_0": "“Na suddhi secanenatthi,",
  "ja487:8.2_0": "nāpi kevalī brāhmaṇo;",
  "ja487:8.3_0": "Na khantī nāpi soraccaṁ,",
  "ja487:8.4_0": "nāpi so parinibbuto”.",
  "ja487:9.1_0": "“Kathaṁ so brāhmaṇo hoti,",
  "ja487:9.2_0": "kathaṁ bhavati kevalī;",
  "ja487:9.3_0": "Kathañca parinibbānaṁ,",
  "ja487:9.4_0": "dhammaṭṭho kinti vuccati”.",
  "ja487:10.1_0": "“Akhettabandhū amamo nirāso,",
  "ja487:10.2_0": "Nillobhapāpo bhavalobhakhīṇo;",
  "ja487:10.3_0": "Evaṅkaro brāhmaṇo hoti khemī,",
  "ja487:10.4_0": "Dhamme ṭhitaṁ tena amāpayiṁsu”.",
  "ja487:11.1_0": "“Khattiyā brāhmaṇā vessā,",
  "ja487:11.2_0": "suddā caṇḍālapukkusā;",
  "ja487:11.3_0": "Sabbeva soratā dantā,",
  "ja487:11.4_0": "sabbeva parinibbutā;",
  "ja487:11.5_0": "Sabbesaṁ sītibhūtānaṁ,",
  "ja487:11.6_0": "atthi seyyotha pāpiyo”.",
  "ja487:12.1_0": "“Khattiyā brāhmaṇā vessā,",
  "ja487:12.2_0": "suddā caṇḍālapukkusā;",
  "ja487:12.3_0": "Sabbeva soratā dantā,",
  "ja487:12.4_0": "sabbeva parinibbutā;",
  "ja487:12.5_0": "Sabbesaṁ sītibhūtānaṁ,",
  "ja487:12.6_0": "natthi seyyotha pāpiyo”.",
  "ja487:13.1_0": "“Khattiyā brāhmaṇā vessā,",
  "ja487:13.2_0": "suddā caṇḍālapukkusā;",
  "ja487:13.3_0": "Sabbeva soratā dantā,",
  "ja487:13.4_0": "sabbeva parinibbutā.",
  "ja487:14.1_0": "Sabbesaṁ sītibhūtānaṁ,",
  "ja487:14.2_0": "Natthi seyyotha pāpiyo;",
  "ja487:14.3_0": "Panatthaṁ carasi brahmaññaṁ,",
  "ja487:14.4_0": "Sottiyākulavaṁsataṁ”.",
  "ja487:15.1_0": "“Nānārattehi vatthehi,",
  "ja487:15.2_0": "vimānaṁ bhavati chāditaṁ;",
  "ja487:15.3_0": "Na tesaṁ chāyā vatthānaṁ,",
  "ja487:15.4_0": "so rāgo anupajjatha.",
  "ja487:16.1_0": "Evameva manussesu,",
  "ja487:16.2_0": "yadā sujjhanti māṇavā;",
  "ja487:16.3_0": "Te sajātiṁ pamuñcanti,",
  "ja487:16.4_0": "dhammamaññāya subbatā”ti.",
  "ja487:17.1_0": "Uddālakajātakaṁ catutthaṁ."
}