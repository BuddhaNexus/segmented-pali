{
  "tha-ap226:0.1_0": "Therāpadāna",
  "tha-ap226:0.2_0": "Ālambaṇadāyakavagga",
  "tha-ap226:0.3_0": "4. Ārakkhadāyakattheraapadāna",
  "tha-ap226:1.1_0": "“Siddhatthassa bhagavato,",
  "tha-ap226:1.2_0": "vedi kārāpitā mayā;",
  "tha-ap226:1.3_0": "Ārakkho ca mayā dinno,",
  "tha-ap226:1.4_0": "sugatassa mahesino.",
  "tha-ap226:2.1_0": "Tena kammavisesena,",
  "tha-ap226:2.2_0": "na passiṁ bhayabheravaṁ;",
  "tha-ap226:2.3_0": "Kuhiñci upapannassa,",
  "tha-ap226:2.4_0": "tāso mayhaṁ na vijjati.",
  "tha-ap226:3.1_0": "Catunnavutito kappe,",
  "tha-ap226:3.2_0": "yaṁ vediṁ kārayiṁ pure;",
  "tha-ap226:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap226:3.4_0": "vedikāya idaṁ phalaṁ.",
  "tha-ap226:4.1_0": "Ito chaṭṭhamhi kappamhi,",
  "tha-ap226:4.2_0": "apassenasanāmako;",
  "tha-ap226:4.3_0": "Sattaratanasampanno,",
  "tha-ap226:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap226:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap226:5.2_0": "…pe…",
  "tha-ap226:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap226:6.1_0": "Itthaṁ sudaṁ āyasmā ārakkhadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap226:7.1_0": "Ārakkhadāyakattherassāpadānaṁ catutthaṁ."
}