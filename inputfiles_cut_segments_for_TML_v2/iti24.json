{
  "iti24:0.1_0": "Itivuttaka 24",
  "iti24:0.2_0": "Ekakanipāta",
  "iti24:0.3_0": "Tatiyavagga",
  "iti24:0.4_0": "Aṭṭhipuñjasutta",
  "iti24:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti24:2.1_0": "“Ekapuggalassa, bhikkhave, kappaṁ sandhāvato saṁsarato siyā evaṁ mahā aṭṭhikaṅkalo aṭṭhipuñjo aṭṭhirāsi yathāyaṁ vepullo pabbato, sace saṁhārako assa, sambhatañca na vinasseyyā”ti.",
  "iti24:2.2_0": "Etamatthaṁ bhagavā avoca.",
  "iti24:2.3_0": "Tatthetaṁ iti vuccati:",
  "iti24:3.1_0": "“Ekassekena kappena,",
  "iti24:3.2_0": "puggalassaṭṭhisañcayo;",
  "iti24:3.3_0": "Siyā pabbatasamo rāsi,",
  "iti24:3.4_0": "iti vuttaṁ mahesinā.",
  "iti24:4.1_0": "So kho panāyaṁ akkhāto,",
  "iti24:4.2_0": "vepullo pabbato mahā;",
  "iti24:4.3_0": "Uttaro gijjhakūṭassa,",
  "iti24:4.4_0": "magadhānaṁ giribbaje.",
  "iti24:5.1_0": "Yato ca ariyasaccāni,",
  "iti24:5.2_0": "sammappaññāya passati;",
  "iti24:5.3_0": "Dukkhaṁ dukkhasamuppādaṁ,",
  "iti24:5.4_0": "dukkhassa ca atikkamaṁ;",
  "iti24:5.5_0": "Ariyañcaṭṭhaṅgikaṁ maggaṁ,",
  "iti24:5.6_0": "dukkhūpasamagāminaṁ.",
  "iti24:6.1_0": "Sa sattakkhattuṁ paramaṁ,",
  "iti24:6.2_0": "sandhāvitvāna puggalo;",
  "iti24:6.3_0": "Dukkhassantakaro hoti,",
  "iti24:6.4_0": "sabbasaṁyojanakkhayā”ti.",
  "iti24:7.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti24:7.2_0": "Catutthaṁ."
}