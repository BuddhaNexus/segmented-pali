{
  "thi-ap30:0.1_0": "Therīapadāna",
  "thi-ap30:0.2_0": "Kuṇḍalakesīvagga",
  "thi-ap30:0.3_0": "10 Yasodharāpamukhaaṭṭhārasabhikkhunīsahassaapadāna",
  "thi-ap30:1.1_0": "Aṭṭhārasasahassāni,",
  "thi-ap30:1.2_0": "bhikkhunī sakyasambhavā;",
  "thi-ap30:1.3_0": "Yasodharāpamukhāni,",
  "thi-ap30:1.4_0": "sambuddhaṁ upasaṅkamuṁ.",
  "thi-ap30:2.1_0": "Aṭṭhārasasahassāni,",
  "thi-ap30:2.2_0": "sabbā honti mahiddhikā;",
  "thi-ap30:2.3_0": "Vandantī munino pāde,",
  "thi-ap30:2.4_0": "ārocenti yathābalaṁ.",
  "thi-ap30:3.1_0": "“Jāti khīṇā jarā byādhi,",
  "thi-ap30:3.2_0": "maraṇañca mahāmuni;",
  "thi-ap30:3.3_0": "Anāsavaṁ padaṁ santaṁ,",
  "thi-ap30:3.4_0": "amataṁ yāma nāyaka.",
  "thi-ap30:4.1_0": "Khalitañce pure atthi,",
  "thi-ap30:4.2_0": "sabbāsampi mahāmuni;",
  "thi-ap30:4.3_0": "Aparādhamajānantī,",
  "thi-ap30:4.4_0": "khama amhaṁ vināyaka”.",
  "thi-ap30:5.1_0": "“Iddhiñcāpi nidassetha,",
  "thi-ap30:5.2_0": "mama sāsanakārikā;",
  "thi-ap30:5.3_0": "Parisānañca sabbāsaṁ,",
  "thi-ap30:5.4_0": "kaṅkhaṁ chindatha yāvatā”.",
  "thi-ap30:6.1_0": "“Yasodharā mahāvīra,",
  "thi-ap30:6.2_0": "manāpā piyadassanā;",
  "thi-ap30:6.3_0": "Sabbā tuyhaṁ mahāvīra,",
  "thi-ap30:6.4_0": "agārasmiṁ pajāpati.",
  "thi-ap30:7.1_0": "Thīnaṁ satasahassānaṁ,",
  "thi-ap30:7.2_0": "navutīnaṁ chaduttari;",
  "thi-ap30:7.3_0": "Agāre te mayaṁ vīra,",
  "thi-ap30:7.4_0": "pāmokkhā sabbā issarā.",
  "thi-ap30:8.1_0": "Rūpācāraguṇūpetā,",
  "thi-ap30:8.2_0": "yobbanaṭṭhā piyaṁvadā;",
  "thi-ap30:8.3_0": "Sabbā no apacāyanti,",
  "thi-ap30:8.4_0": "devatā viya mānusā.",
  "thi-ap30:9.1_0": "Aṭṭhārasasahassāni,",
  "thi-ap30:9.2_0": "sabbā sākiyasambhavā;",
  "thi-ap30:9.3_0": "Yasodharāsahassāni,",
  "thi-ap30:9.4_0": "pāmokkhā issarā tadā.",
  "thi-ap30:10.1_0": "Kāmadhātumatikkamma,",
  "thi-ap30:10.2_0": "saṇṭhitā rūpadhātuyā;",
  "thi-ap30:10.3_0": "Rūpena sadisā natthi,",
  "thi-ap30:10.4_0": "sahassānaṁ mahāmuni”.",
  "thi-ap30:11.1_0": "Sambuddhaṁ abhivādetvā,",
  "thi-ap30:11.2_0": "iddhiṁ dassaṁsu satthuno;",
  "thi-ap30:11.3_0": "Nekā nānāvidhākārā,",
  "thi-ap30:11.4_0": "mahāiddhīpi dassayuṁ.",
  "thi-ap30:12.1_0": "Cakkavāḷasamaṁ kāyaṁ,",
  "thi-ap30:12.2_0": "sīsaṁ uttarato kuru;",
  "thi-ap30:12.3_0": "Ubho pakkhā duve dīpā,",
  "thi-ap30:12.4_0": "jambudīpaṁ sarīrato.",
  "thi-ap30:13.1_0": "Dakkhiṇañca saraṁ piñchaṁ,",
  "thi-ap30:13.2_0": "nānāsākhā tu pattakā;",
  "thi-ap30:13.3_0": "Candañca sūriyañcakkhi,",
  "thi-ap30:13.4_0": "merupabbatato sikhaṁ.",
  "thi-ap30:14.1_0": "Cakkavāḷagiriṁ tuṇḍaṁ,",
  "thi-ap30:14.2_0": "jamburukkhaṁ samūlakaṁ;",
  "thi-ap30:14.3_0": "Bījamānā upāgantvā,",
  "thi-ap30:14.4_0": "vandantī lokanāyakaṁ.",
  "thi-ap30:15.1_0": "Hatthivaṇṇaṁ tathevassaṁ,",
  "thi-ap30:15.2_0": "pabbataṁ jaladhiṁ tathā;",
  "thi-ap30:15.3_0": "Candañca sūriyaṁ meruṁ,",
  "thi-ap30:15.4_0": "sakkavaṇṇañca dassayuṁ.",
  "thi-ap30:16.1_0": "“Yasodharā mayaṁ vīra,",
  "thi-ap30:16.2_0": "pāde vandāma cakkhuma;",
  "thi-ap30:16.3_0": "Tava cirapabhāvena,",
  "thi-ap30:16.4_0": "nipphannā naranāyaka.",
  "thi-ap30:17.1_0": "Iddhīsu ca vasī homa,",
  "thi-ap30:17.2_0": "dibbāya sotadhātuyā;",
  "thi-ap30:17.3_0": "Cetopariyañāṇassa,",
  "thi-ap30:17.4_0": "vasī homa mahāmune.",
  "thi-ap30:18.1_0": "Pubbenivāsaṁ jānāmi,",
  "thi-ap30:18.2_0": "dibbacakkhu visodhitaṁ;",
  "thi-ap30:18.3_0": "Sabbāsavā parikkhīṇā,",
  "thi-ap30:18.4_0": "natthi dāni punabbhavo.",
  "thi-ap30:19.1_0": "Atthadhammaniruttīsu,",
  "thi-ap30:19.2_0": "paṭibhāne tatheva ca;",
  "thi-ap30:19.3_0": "Ñāṇaṁ amhaṁ mahāvīra,",
  "thi-ap30:19.4_0": "uppannaṁ tava santike.",
  "thi-ap30:20.1_0": "Pubbānaṁ lokanāthānaṁ,",
  "thi-ap30:20.2_0": "saṅgamaṁ no nidassitaṁ;",
  "thi-ap30:20.3_0": "Adhikārā bahū amhaṁ,",
  "thi-ap30:20.4_0": "tuyhatthāya mahāmune.",
  "thi-ap30:21.1_0": "Yaṁ amhaṁ pūritaṁ kammaṁ,",
  "thi-ap30:21.2_0": "kusalaṁ sarase mune;",
  "thi-ap30:21.3_0": "Tuyhatthāya mahāvīra,",
  "thi-ap30:21.4_0": "puññānupacitāni no.",
  "thi-ap30:22.1_0": "Abhabbaṭṭhāne vajjetvā,",
  "thi-ap30:22.2_0": "vārayimha anācaraṁ;",
  "thi-ap30:22.3_0": "Tuyhatthāya mahāvīra,",
  "thi-ap30:22.4_0": "cattāni jīvitāni no.",
  "thi-ap30:23.1_0": "Nekakoṭisahassāni,",
  "thi-ap30:23.2_0": "bhariyatthāyadāsi no;",
  "thi-ap30:23.3_0": "Na tattha vimanā homa,",
  "thi-ap30:23.4_0": "tuyhatthāya mahāmune.",
  "thi-ap30:24.1_0": "Nekakoṭisahassāni,",
  "thi-ap30:24.2_0": "upakārāyadāsi no;",
  "thi-ap30:24.3_0": "Na tattha vimanā homa,",
  "thi-ap30:24.4_0": "tuyhatthāya mahāmune.",
  "thi-ap30:25.1_0": "Nekakoṭisahassāni,",
  "thi-ap30:25.2_0": "bhojanatthāyadāsi no;",
  "thi-ap30:25.3_0": "Na tattha vimanā homa,",
  "thi-ap30:25.4_0": "tuyhatthāya mahāmune.",
  "thi-ap30:26.1_0": "Nekakoṭisahassāni,",
  "thi-ap30:26.2_0": "jīvitāni cajimhase;",
  "thi-ap30:26.3_0": "Bhayamokkhaṁ karissāma,",
  "thi-ap30:26.4_0": "jīvitāni cajimhase.",
  "thi-ap30:27.1_0": "Aṅgagate alaṅkāre,",
  "thi-ap30:27.2_0": "vatthe nānāvidhe bahū;",
  "thi-ap30:27.3_0": "Itthibhaṇḍe na gūhāma,",
  "thi-ap30:27.4_0": "tuyhatthāya mahāmune.",
  "thi-ap30:28.1_0": "Dhanadhaññapariccāgaṁ,",
  "thi-ap30:28.2_0": "gāmāni nigamāni ca;",
  "thi-ap30:28.3_0": "Khettaṁ puttā ca dhītā ca,",
  "thi-ap30:28.4_0": "pariccattā mahāmune.",
  "thi-ap30:29.1_0": "Hatthī assā gavā cāpi,",
  "thi-ap30:29.2_0": "dāsiyo paricārikā;",
  "thi-ap30:29.3_0": "Tuyhatthāya mahāvīra,",
  "thi-ap30:29.4_0": "pariccattaṁ asaṅkhiyaṁ.",
  "thi-ap30:30.1_0": "Yaṁ amhe paṭimantesi,",
  "thi-ap30:30.2_0": "‘dānaṁ dassāma yācake’;",
  "thi-ap30:30.3_0": "Vimanaṁ no na passāma,",
  "thi-ap30:30.4_0": "dadato dānamuttamaṁ.",
  "thi-ap30:31.1_0": "Nānāvidhaṁ bahuṁ dukkhaṁ,",
  "thi-ap30:31.2_0": "saṁsāre ca bahubbidhe;",
  "thi-ap30:31.3_0": "Tuyhatthāya mahāvīra,",
  "thi-ap30:31.4_0": "anubhuttaṁ asaṅkhiyaṁ.",
  "thi-ap30:32.1_0": "Sukhappattānumodāma,",
  "thi-ap30:32.2_0": "na ca dukkhesu dummanā;",
  "thi-ap30:32.3_0": "Sabbattha tulitā homa,",
  "thi-ap30:32.4_0": "tuyhatthāya mahāmune.",
  "thi-ap30:33.1_0": "Anumaggena sambuddho,",
  "thi-ap30:33.2_0": "yaṁ dhammaṁ abhinīhari;",
  "thi-ap30:33.3_0": "Anubhotvā sukhaṁ dukkhaṁ,",
  "thi-ap30:33.4_0": "patto bodhiṁ mahāmune.",
  "thi-ap30:34.1_0": "Brahmadevañca sambuddhaṁ,",
  "thi-ap30:34.2_0": "gotamaṁ lokanāyakaṁ;",
  "thi-ap30:34.3_0": "Aññesaṁ lokanāthānaṁ,",
  "thi-ap30:34.4_0": "saṅgamaṁ tehi no bahū.",
  "thi-ap30:35.1_0": "Adhikāraṁ bahuṁ amhe,",
  "thi-ap30:35.2_0": "tuyhatthāya mahāmune;",
  "thi-ap30:35.3_0": "Gavesato buddhadhamme,",
  "thi-ap30:35.4_0": "mayaṁ te paricārikā.",
  "thi-ap30:36.1_0": "Kappe ca satasahasse,",
  "thi-ap30:36.2_0": "caturo ca asaṅkhiye;",
  "thi-ap30:36.3_0": "Dīpaṅkaro mahāvīro,",
  "thi-ap30:36.4_0": "uppajji lokanāyako.",
  "thi-ap30:37.1_0": "Paccantadesavisaye,",
  "thi-ap30:37.2_0": "nimantetvā tathāgataṁ;",
  "thi-ap30:37.3_0": "Tassa āgamanaṁ maggaṁ,",
  "thi-ap30:37.4_0": "sodhenti tuṭṭhamānasā.",
  "thi-ap30:38.1_0": "Tena kālena so āsi,",
  "thi-ap30:38.2_0": "sumedho nāma brāhmaṇo;",
  "thi-ap30:38.3_0": "Maggañca paṭiyādesi,",
  "thi-ap30:38.4_0": "āyato sabbadassino.",
  "thi-ap30:39.1_0": "Tena kālena ahumha,",
  "thi-ap30:39.2_0": "sabbā brāhmaṇasambhavā;",
  "thi-ap30:39.3_0": "Thalūdajāni pupphāni,",
  "thi-ap30:39.4_0": "āharimha samāgamaṁ.",
  "thi-ap30:40.1_0": "Tasmiṁ so samaye buddho,",
  "thi-ap30:40.2_0": "dīpaṅkaro mahāyaso;",
  "thi-ap30:40.3_0": "Viyākāsi mahāvīro,",
  "thi-ap30:40.4_0": "isimuggatamānasaṁ.",
  "thi-ap30:41.1_0": "Calatī ravatī puthavī,",
  "thi-ap30:41.2_0": "saṅkampati sadevake;",
  "thi-ap30:41.3_0": "Tassa kammaṁ pakittente,",
  "thi-ap30:41.4_0": "isimuggatamānasaṁ.",
  "thi-ap30:42.1_0": "Devakaññā manussā ca,",
  "thi-ap30:42.2_0": "mayañcāpi sadevakā;",
  "thi-ap30:42.3_0": "Nānāpūjanīyaṁ bhaṇḍaṁ,",
  "thi-ap30:42.4_0": "pūjayitvāna patthayuṁ.",
  "thi-ap30:43.1_0": "Tesaṁ buddho viyākāsi,",
  "thi-ap30:43.2_0": "jotidīpa sanāmako;",
  "thi-ap30:43.3_0": "‘Ajja ye patthitā atthi,",
  "thi-ap30:43.4_0": "te bhavissanti sammukhā’.",
  "thi-ap30:44.1_0": "Aparimeyye ito kappe,",
  "thi-ap30:44.2_0": "yaṁ no buddho viyākari;",
  "thi-ap30:44.3_0": "Taṁ vācamanumodentā,",
  "thi-ap30:44.4_0": "evaṅkārī ahumha no.",
  "thi-ap30:45.1_0": "Tassa kammassa sukatassa,",
  "thi-ap30:45.2_0": "tassa cittaṁ pasādayuṁ;",
  "thi-ap30:45.3_0": "Devamānusikaṁ yoniṁ,",
  "thi-ap30:45.4_0": "anubhotvā asaṅkhiyaṁ.",
  "thi-ap30:46.1_0": "Sukhadukkhenubhotvāna,",
  "thi-ap30:46.2_0": "devesu mānusesu ca;",
  "thi-ap30:46.3_0": "Pacchime bhave sampatte,",
  "thi-ap30:46.4_0": "jātāmha sākiye kule.",
  "thi-ap30:47.1_0": "Rūpavatī bhogavatī,",
  "thi-ap30:47.2_0": "yasasīlavatī tato;",
  "thi-ap30:47.3_0": "Sabbaṅgasampadā homa,",
  "thi-ap30:47.4_0": "kulesu abhisakkatā.",
  "thi-ap30:48.1_0": "Lābhaṁ silokaṁ sakkāraṁ,",
  "thi-ap30:48.2_0": "lokadhammasamāgamaṁ;",
  "thi-ap30:48.3_0": "Cittañca dukkhitaṁ natthi,",
  "thi-ap30:48.4_0": "vasāma akutobhayā.",
  "thi-ap30:49.1_0": "Vuttañhetaṁ bhagavatā,",
  "thi-ap30:49.2_0": "‘rañño antepure tadā;",
  "thi-ap30:49.3_0": "Khattiyānaṁ pure vīra,",
  "thi-ap30:49.4_0": "upakārañca niddisi.",
  "thi-ap30:50.1_0": "Upakārā ca yā nārī,",
  "thi-ap30:50.2_0": "yā ca nārī sukhe dukhe;",
  "thi-ap30:50.3_0": "Atthakkhāyī ca yā nārī,",
  "thi-ap30:50.4_0": "yā ca nārīnukampikā.",
  "thi-ap30:51.1_0": "Dhammaṁ care sucaritaṁ,",
  "thi-ap30:51.2_0": "na naṁ duccaritaṁ care;",
  "thi-ap30:51.3_0": "Dhammacārī sukhaṁ seti,",
  "thi-ap30:51.4_0": "asmiṁ loke paramhi ca’.",
  "thi-ap30:52.1_0": "Agāraṁ vijahitvāna,",
  "thi-ap30:52.2_0": "pabbajimhanagāriyaṁ;",
  "thi-ap30:52.3_0": "Aḍḍhamāse asampatte,",
  "thi-ap30:52.4_0": "catusaccaṁ phusimha no.",
  "thi-ap30:53.1_0": "Cīvaraṁ piṇḍapātañca,",
  "thi-ap30:53.2_0": "paccayaṁ sayanāsanaṁ;",
  "thi-ap30:53.3_0": "Upanenti bahū amhe,",
  "thi-ap30:53.4_0": "sāgarasseva ūmiyo.",
  "thi-ap30:54.1_0": "Kilesā jhāpitā amhaṁ,",
  "thi-ap30:54.2_0": "bhavā sabbe samūhatā;",
  "thi-ap30:54.3_0": "Nāgīva bandhanaṁ chetvā,",
  "thi-ap30:54.4_0": "viharāma anāsavā.",
  "thi-ap30:55.1_0": "Svāgataṁ vata no āsi,",
  "thi-ap30:55.2_0": "mama buddhassa santike;",
  "thi-ap30:55.3_0": "Tisso vijjā anuppattā,",
  "thi-ap30:55.4_0": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap30:56.1_0": "Paṭisambhidā catasso,",
  "thi-ap30:56.2_0": "vimokkhāpi ca aṭṭhime;",
  "thi-ap30:56.3_0": "Chaḷabhiññā sacchikatā,",
  "thi-ap30:56.4_0": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap30:57.1_0": "Evaṁ bahuvidhaṁ dukkhaṁ,",
  "thi-ap30:57.2_0": "sampattī ca bahubbidhā;",
  "thi-ap30:57.3_0": "Visuddhabhāvaṁ sampattā,",
  "thi-ap30:57.4_0": "labhāma sabbasampadā.",
  "thi-ap30:58.1_0": "Yā dadanti sakattānaṁ,",
  "thi-ap30:58.2_0": "puññatthāya mahesino;",
  "thi-ap30:58.3_0": "Sahāyasampadā honti,",
  "thi-ap30:58.4_0": "nibbānapadamasaṅkhataṁ.",
  "thi-ap30:59.1_0": "Parikkhīṇaṁ atītañca,",
  "thi-ap30:59.2_0": "paccuppannaṁ anāgataṁ;",
  "thi-ap30:59.3_0": "Sabbakammampi no khīṇaṁ,",
  "thi-ap30:59.4_0": "pāde vandāma cakkhuma”.",
  "thi-ap30:60.1_0": "“Nibbānāya vadantīnaṁ,",
  "thi-ap30:60.2_0": "Kiṁ vo vakkhāma uttari;",
  "thi-ap30:60.3_0": "Santasaṅkhatadosañhi,",
  "thi-ap30:60.4_0": "Pappotha amataṁ padaṁ”.",
  "thi-ap30:61.1_0": "Itthaṁ sudaṁ yasodharāpamukhāni aṭṭhārasabhikkhunīsahassāni bhagavato sammukhā imā gāthāyo abhāsitthāti.",
  "thi-ap30:62.1_0": "Yasodharāpamukhaaṭṭhārasabhikkhunīsahassāpadānaṁ dasamaṁ.",
  "thi-ap30:63.1_0": "Kuṇḍalakesīvaggo tatiyo.",
  "thi-ap30:64.1_0": "Tassuddānaṁ",
  "thi-ap30:65.1_0": "Kuṇḍalā gotamī ceva,",
  "thi-ap30:65.2_0": "dhammadinnā ca sakulā;",
  "thi-ap30:65.3_0": "Varanandā ca soṇā ca,",
  "thi-ap30:65.4_0": "kāpilānī yasodharā.",
  "thi-ap30:66.1_0": "Dasasahassabhikkhunī,",
  "thi-ap30:66.2_0": "aṭṭhārasasahassakā;",
  "thi-ap30:66.3_0": "Gāthāsatāni cattāri,",
  "thi-ap30:66.4_0": "cha ca sattatimeva ca."
}