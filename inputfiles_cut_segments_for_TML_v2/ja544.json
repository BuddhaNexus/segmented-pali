  "ja544:87.3_0": "Yajatettha ekarājā,",
  "ja544:87.4_0": "Sammūḷho catūhi usabhehi.",
  "ja544:88.1_0": "Yadi sakuṇi maṁsamicchasi,",
  "ja544:88.2_0": "Ḍayassu pubbena pupphavatiyā;",
  "ja544:88.3_0": "Yajatettha ekarājā,",
  "ja544:88.4_0": "Sammūḷho sabbacatukkena.",
  "ja544:89.1_0": "Ayamassa pāsādo,",
  "ja544:89.2_0": "Idaṁ antepuraṁ suramaṇīyaṁ;",
  "ja544:89.3_0": "Tedāni ayyaputtā,",
  "ja544:89.4_0": "Cattāro vadhāya ninnītā.",
  "ja544:90.1_0": "Idamassa kūṭāgāraṁ,",
  "ja544:90.2_0": "Sovaṇṇaṁ pupphamalyavikiṇṇaṁ;",
  "ja544:90.3_0": "Tedāni ayyaputtā,",
  "ja544:90.4_0": "Cattāro vadhāya ninnītā.",
  "ja544:91.1_0": "Idamassa uyyānaṁ,",
  "ja544:91.2_0": "Supupphitaṁ sabbakālikaṁ rammaṁ;",
  "ja544:91.3_0": "Tedāni ayyaputtā,",
  "ja544:91.4_0": "Cattāro vadhāya ninnītā.",
  "ja544:92.1_0": "Idamassa asokavanaṁ,",
  "ja544:92.2_0": "Supupphitaṁ sabbakālikaṁ rammaṁ;",
