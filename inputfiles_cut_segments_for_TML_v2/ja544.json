  "ja544:92.4_0": "Cattāro vadhāya ninnītā.",
  "ja544:93.1_0": "Idamassa kaṇikāravanaṁ,",
  "ja544:93.2_0": "Supupphitaṁ sabbakālikaṁ rammaṁ;",
  "ja544:93.3_0": "Tedāni ayyaputtā,",
  "ja544:93.4_0": "Cattāro vadhāya ninnītā.",
  "ja544:94.1_0": "Idamassa pāṭalivanaṁ,",
  "ja544:94.2_0": "Supupphitaṁ sabbakālikaṁ rammaṁ;",
  "ja544:94.3_0": "Tedāni ayyaputtā,",
  "ja544:94.4_0": "Cattāro vadhāya ninnītā.",
  "ja544:95.1_0": "Idamassa ambavanaṁ,",
  "ja544:95.2_0": "Supupphitaṁ sabbakālikaṁ rammaṁ;",
  "ja544:95.3_0": "Tedāni ayyaputtā,",
  "ja544:95.4_0": "Cattāro vadhāya ninnītā.",
  "ja544:96.1_0": "Ayamassa pokkharaṇī,",
  "ja544:96.2_0": "Sañchannā padumapuṇḍarīkehi;",
  "ja544:96.3_0": "Nāvā ca sovaṇṇavikatā,",
  "ja544:96.4_0": "Pupphavalliyā cittā suramaṇīyā;",
  "ja544:96.5_0": "Tedāni ayyaputtā,",
  "ja544:96.6_0": "Cattāro vadhāya ninnītā.",
  "ja544:97.1_0": "Idamassa hatthiratanaṁ,",
  "ja544:97.2_0": "Erāvaṇo gajo balī dantī;",
  "ja544:97.3_0": "Tedāni ayyaputtā,",
  "ja544:97.4_0": "Cattāro vadhāya ninnītā.",
  "ja544:98.1_0": "Idamassa assaratanaṁ,",
  "ja544:98.2_0": "ekakhuro asso;",
  "ja544:98.3_0": "Tedāni ayyaputtā,",
  "ja544:98.4_0": "cattāro vadhāya ninnītā.",
  "ja544:99.1_0": "Ayamassa assaratho,",
  "ja544:99.2_0": "Sāḷiya nigghoso subho ratanavicitto;",
  "ja544:99.3_0": "Yatthassu ayyaputtā,",
  "ja544:99.4_0": "Sobhiṁsu nandane viya devā;",
  "ja544:99.5_0": "Tedāni ayyaputtā,",
  "ja544:99.6_0": "Cattāro vadhāya ninnītā.",
  "ja544:100.1_0": "Kathaṁ nāma sāmasamasundarehi,",
  "ja544:100.2_0": "Candanamudukagattehi;",
  "ja544:100.3_0": "Rājā yajissate yaññaṁ,",
  "ja544:100.4_0": "Sammūḷho catūhi puttehi.",
  "ja544:101.1_0": "Kathaṁ nāma sāmasamasundarāhi,",
  "ja544:101.2_0": "Candanamudukagattāhi;",
  "ja544:101.3_0": "Rājā yajissate yaññaṁ,",
  "ja544:101.4_0": "Sammūḷho catūhi kaññāhi.",
  "ja544:102.1_0": "Kathaṁ nāma sāmasamasundarāhi,",
  "ja544:102.2_0": "Candanamudukagattāhi;",
  "ja544:102.3_0": "Rājā yajissate yaññaṁ,",
  "ja544:102.4_0": "Sammūḷho catūhi mahesīhi.",
  "ja544:103.1_0": "Kathaṁ nāma sāmasamasundarehi,",
  "ja544:103.2_0": "Candanamudukagattehi;",
  "ja544:103.3_0": "Rājā yajissate yaññaṁ,",
  "ja544:103.4_0": "Sammūḷho catūhi gahapatīhi.",
  "ja544:104.1_0": "Yathā honti gāmanigamā,",
  "ja544:104.2_0": "Suññā amanussakā brahāraññā;",
  "ja544:104.3_0": "Tathā hessati pupphavatiyā,",
  "ja544:104.4_0": "Yiṭṭhesu candasūriyesu”.",
  "ja544:105.1_0": "“Ummattikā bhavissāmi,",
  "ja544:105.2_0": "Bhūnahatā paṁsunā ca parikiṇṇā;",
  "ja544:105.3_0": "Sace candavaraṁ hanti,",
  "ja544:105.4_0": "Pāṇā me deva rujjhanti.",
  "ja544:106.1_0": "Ummattikā bhavissāmi,",
  "ja544:106.2_0": "Bhūnahatā paṁsunā ca parikiṇṇā;",
  "ja544:106.3_0": "Sace sūriyavaraṁ hanti,",
  "ja544:106.4_0": "Pāṇā me deva rujjhanti”.",
  "ja544:107.1_0": "“Kinnumā na ramāpeyyuṁ,",
  "ja544:107.2_0": "Aññamaññaṁ piyaṁvadā;",
  "ja544:107.3_0": "Ghaṭṭikā uparikkhī ca,",
  "ja544:107.4_0": "Pokkharaṇī ca bhārikā;",
  "ja544:107.5_0": "Candasūriyesu naccantiyo,",
  "ja544:107.6_0": "Samā tāsaṁ na vijjati”.",
  "ja544:108.1_0": "“Imaṁ mayhaṁ hadayasokaṁ,",
  "ja544:108.2_0": "Paṭimuñcatu khaṇḍahāla tava mātā;",
  "ja544:108.3_0": "Yo mayhaṁ hadayasoko,",
  "ja544:108.4_0": "Candamhi vadhāya ninnīte.",
  "ja544:109.1_0": "Imaṁ mayhaṁ hadayasokaṁ,",
  "ja544:109.2_0": "Paṭimuñcatu khaṇḍahāla tava mātā;",
  "ja544:109.3_0": "Yo mayhaṁ hadayasoko,",
  "ja544:109.4_0": "Sūriyamhi vadhāya ninnīte.",
  "ja544:110.1_0": "Imaṁ mayhaṁ hadayasokaṁ,",
  "ja544:110.2_0": "Paṭimuñcatu khaṇḍahāla tava jāyā;",
  "ja544:110.3_0": "Yo mayhaṁ hadayasoko,",
  "ja544:110.4_0": "Candamhi vadhāya ninnīte.",
  "ja544:111.1_0": "Imaṁ mayhaṁ hadayasokaṁ,",
  "ja544:111.2_0": "Paṭimuñcatu khaṇḍahāla tava jāyā;",
  "ja544:111.3_0": "Yo mayhaṁ hadayasoko,",
  "ja544:111.4_0": "Sūriyamhi vadhāya ninnīte.",
  "ja544:112.1_0": "Mā ca putte mā ca patiṁ,",
  "ja544:112.2_0": "Addakkhi khaṇḍahāla tava mātā;",
  "ja544:112.3_0": "Yo ghātesi kumāre,",
  "ja544:112.4_0": "Adūsake sīhasaṅkāse.",
  "ja544:113.1_0": "Mā ca putte mā ca patiṁ,",
  "ja544:113.2_0": "Addakkhi khaṇḍahāla tava mātā;",
  "ja544:113.3_0": "Yo ghātesi kumāre,",
  "ja544:113.4_0": "Apekkhite sabbalokassa.",
  "ja544:114.1_0": "Mā ca putte mā ca patiṁ,",
  "ja544:114.2_0": "Addakkhi khaṇḍahāla tava jāyā;",
  "ja544:114.3_0": "Yo ghātesi kumāre,",
  "ja544:114.4_0": "Adūsake sīhasaṅkāse.",
  "ja544:115.1_0": "Mā ca putte mā ca patiṁ,",
  "ja544:115.2_0": "Addakkhi khaṇḍahāla tava jāyā;",
  "ja544:115.3_0": "Yo ghātesi kumāre,",
  "ja544:115.4_0": "Apekkhite sabbalokassa”.",
  "ja544:116.1_0": "“Mā no deva avadhi,",
  "ja544:116.2_0": "Dāse no dehi khaṇḍahālassa;",
  "ja544:116.3_0": "Api nigaḷabandhakāpi,",
  "ja544:116.4_0": "Hatthī asse ca pālema.",
  "ja544:117.1_0": "Mā no deva avadhi,",
  "ja544:117.2_0": "Dāse no dehi khaṇḍahālassa;",
  "ja544:117.3_0": "Api nigaḷabandhakāpi,",
  "ja544:117.4_0": "Hatthichakaṇāni ujjhema.",
  "ja544:118.1_0": "Mā no deva avadhi,",
  "ja544:118.2_0": "Dāse no dehi khaṇḍahālassa;",
  "ja544:118.3_0": "Api nigaḷabandhakāpi,",
  "ja544:118.4_0": "Assachakaṇāni ujjhema.",
  "ja544:119.1_0": "Mā no deva avadhi,",
  "ja544:119.2_0": "Dāse no dehi khaṇḍahālassa;",
  "ja544:119.3_0": "Yassa honti tava kāmā,",
  "ja544:119.4_0": "Api raṭṭhā pabbājitā;",
  "ja544:119.5_0": "Bhikkhācariyaṁ carissāma.",
  "ja544:120.1_0": "Dibbaṁ deva upayācanti,",
  "ja544:120.2_0": "puttatthikāpi daliddā;",
  "ja544:120.3_0": "Paṭibhānānipi hitvā,",
  "ja544:120.4_0": "putte na labhanti ekaccā.",
  "ja544:121.1_0": "Āsīsikāni karonti,",
  "ja544:121.2_0": "Puttā no jāyantu tato paputtā;",
  "ja544:121.3_0": "Atha no akāraṇasmā,",
  "ja544:121.4_0": "Yaññatthāya deva ghātesi.",
  "ja544:122.1_0": "Upayācitakena puttaṁ labhanti,",
  "ja544:122.2_0": "Mā tāta no aghātesi;",
  "ja544:122.3_0": "Mā kicchāladdhakehi puttehi,",
  "ja544:122.4_0": "Yajittho imaṁ yaññaṁ.",
  "ja544:123.1_0": "Upayācitakena puttaṁ labhanti,",
  "ja544:123.2_0": "Mā tāta no aghātesi;",
  "ja544:123.3_0": "Mā kapaṇaladdhakehi puttehi,",
  "ja544:123.4_0": "Ammāya no vippavāsesi.",
  "ja544:124.1_0": "Bahudukkhā posiya candaṁ,",
  "ja544:124.2_0": "amma tuvaṁ jīyase puttaṁ;",
  "ja544:124.3_0": "Vandāmi kho te pāde,",
  "ja544:124.4_0": "labhataṁ tāto paralokaṁ.",
  "ja544:125.1_0": "Handa ca maṁ upaguyha,",
  "ja544:125.2_0": "Pāde te amma vandituṁ dehi;",
  "ja544:125.3_0": "Gacchāmi dāni pavāsaṁ,",
  "ja544:125.4_0": "Yaññatthāya ekarājassa.",
  "ja544:126.1_0": "Handa ca maṁ upagūha,",
  "ja544:126.2_0": "Pāde te amma vandituṁ dehi;",
  "ja544:126.3_0": "Gacchāmi dāni pavāsaṁ,",
  "ja544:126.4_0": "Mātu katvā hadayasokaṁ.",
  "ja544:127.1_0": "Handa ca maṁ upagūha,",
  "ja544:127.2_0": "Pāde te amma vandituṁ dehi;",
  "ja544:127.3_0": "Gacchāmi dāni pavāsaṁ,",
  "ja544:127.4_0": "Janassa katvā hadayasokaṁ”.",
  "ja544:128.1_0": "“Handa ca padumapattānaṁ,",
  "ja544:128.2_0": "Moḷiṁ bandhassu gotamiputta;",
  "ja544:128.3_0": "Campakadalamissāyo,",
  "ja544:128.4_0": "Esā te porāṇikā pakati.",
  "ja544:129.1_0": "Handa ca vilepanaṁ te,",
  "ja544:129.2_0": "Pacchimakaṁ candanaṁ vilimpassu;",
  "ja544:129.3_0": "Yehi ca suvilitto,",
  "ja544:129.4_0": "Sobhasi rājaparisāyaṁ.",
  "ja544:130.1_0": "Handa ca mudukāni vatthāni,",
  "ja544:130.2_0": "Pacchimakaṁ kāsikaṁ nivāsehi;",
  "ja544:130.3_0": "Yehi ca sunivattho,",
  "ja544:130.4_0": "Sobhasi rājaparisāyaṁ.",
  "ja544:131.1_0": "Muttāmaṇikanakavibhūsitāni,",
  "ja544:131.2_0": "Gaṇhassu hatthābharaṇāni;",
  "ja544:131.3_0": "Yehi ca hatthābharaṇehi,",
  "ja544:131.4_0": "Sobhasi rājaparisāyaṁ”.",
  "ja544:132.1_0": "“Na hi nūnāyaṁ raṭṭhapālo,",
  "ja544:132.2_0": "Bhūmipati janapadassa dāyādo;",
  "ja544:132.3_0": "Lokissaro mahanto,",
  "ja544:132.4_0": "Putte snehaṁ janayati”.",
  "ja544:133.1_0": "“Mayhampi piyā puttā,",
  "ja544:133.2_0": "Attā ca piyo tumhe ca bhariyāyo;",
  "ja544:133.3_0": "Saggañca patthayāno,",
  "ja544:133.4_0": "Tenāhaṁ ghātayissāmi”.",
  "ja544:134.1_0": "“Maṁ paṭhamaṁ ghātehi,",
  "ja544:134.2_0": "Mā me hadayaṁ dukkhaṁ phālesi;",
  "ja544:134.3_0": "Alaṅkato sundarako,",
  "ja544:134.4_0": "Putto deva tava sukhumālo.",
  "ja544:135.1_0": "Handayya maṁ hanassu,",
  "ja544:135.2_0": "Paraloke candakena hessāmi;",
  "ja544:135.3_0": "Puññaṁ karassu vipulaṁ,",
  "ja544:135.4_0": "Vicarāma ubhopi paraloke”.",
  "ja544:136.1_0": "“Mā tvaṁ cande rucci maraṇaṁ,",
  "ja544:136.2_0": "Bahukā tava devarā visālakkhi;",
  "ja544:136.3_0": "Te taṁ ramayissanti,",
  "ja544:136.4_0": "Yiṭṭhasmiṁ gotamiputte”.",
  "ja544:137.1_0": "Evaṁ vutte, candā attānaṁ,",
  "ja544:137.2_0": "hanti hatthatalakehi;",
  "ja544:137.3_0": "“Alamettha jīvitena,",
  "ja544:137.4_0": "pissāmi visaṁ marissāmi.",
  "ja544:138.1_0": "Na hi nūnimassa rañño,",
  "ja544:138.2_0": "Mittāmaccā ca vijjare suhadā;",
  "ja544:138.3_0": "Ye na vadanti rājānaṁ,",
  "ja544:138.4_0": "Mā ghātayi orase putte.",
  "ja544:139.1_0": "Na hi nūnimassa rañño,",
  "ja544:139.2_0": "Ñātī mittā ca vijjare suhadā;",
  "ja544:139.3_0": "Ye na vadanti rājānaṁ,",
  "ja544:139.4_0": "Mā ghātayi atraje putte.",
  "ja544:140.1_0": "Ime tepi mayhaṁ puttā,",
  "ja544:140.2_0": "Guṇino kāyūradhārino rāja;",
  "ja544:140.3_0": "Tehipi yajassu yaññaṁ,",
  "ja544:140.4_0": "Atha muñcatu gotamiputte.",
  "ja544:141.1_0": "Bilasataṁ maṁ katvāna,",
  "ja544:141.2_0": "Yajassu sattadhā mahārāja;",
  "ja544:141.3_0": "Mā jeṭṭhaputtamavadhi,",
  "ja544:141.4_0": "Adūsakaṁ sīhasaṅkāsaṁ.",
  "ja544:142.1_0": "Bilasataṁ maṁ katvāna,",
  "ja544:142.2_0": "Yajassu sattadhā mahārāja;",
  "ja544:142.3_0": "Mā jeṭṭhaputtamavadhi,",
  "ja544:142.4_0": "Apekkhitaṁ sabbalokassa”.",
  "ja544:143.1_0": "“Bahukā tava dinnābharaṇā,",
  "ja544:143.2_0": "Uccāvacā subhaṇitamhi;",
  "ja544:143.3_0": "Muttāmaṇiveḷuriyā,",
  "ja544:143.4_0": "Etaṁ te pacchimakaṁ dānaṁ”.",
  "ja544:144.1_0": "“Yesaṁ pubbe khandhesu,",
  "ja544:144.2_0": "Phullā mālāguṇā vivattiṁsu;",
  "ja544:144.3_0": "Tesajjapi sunisito,",
  "ja544:144.4_0": "Nettiṁso vivattissati khandhesu.",
  "ja544:145.1_0": "Yesaṁ pubbe khandhesu,",
  "ja544:145.2_0": "Cittā mālāguṇā vivattiṁsu;",
  "ja544:145.3_0": "Tesajjapi sunisito,",
  "ja544:145.4_0": "Nettiṁso vivattissati khandhesu.",
  "ja544:146.1_0": "Aciraṁ vata nettiṁso,",
  "ja544:146.2_0": "Vivattissati rājaputtānaṁ khandhesu;",
  "ja544:146.3_0": "Atha mama hadayaṁ na phalati,",
  "ja544:146.4_0": "Tāva daḷhabandhañca me āsi.",
  "ja544:147.1_0": "Kāsikasucivatthadharā,",
  "ja544:147.2_0": "Kuṇḍalino agalucandanavilittā;",
  "ja544:147.3_0": "Niyyātha candasūriyā,",
  "ja544:147.4_0": "Yaññatthāya ekarājassa.",
  "ja544:148.1_0": "Kāsikasucivatthadharā,",
  "ja544:148.2_0": "Kuṇḍalino agalucandanavilittā;",
  "ja544:148.3_0": "Niyyātha candasūriyā,",
  "ja544:148.4_0": "Mātu katvā hadayasokaṁ.",
  "ja544:149.1_0": "Kāsikasucivatthadharā,",
  "ja544:149.2_0": "Kuṇḍalino agalucandanavilittā;",
  "ja544:149.3_0": "Niyyātha candasūriyā,",
  "ja544:149.4_0": "Janassa katvā hadayasokaṁ.",
  "ja544:150.1_0": "Maṁsarasabhojanā nhāpakasunhāpitā,",
  "ja544:150.2_0": "Kuṇḍalino agalucandanavilittā;",
  "ja544:150.3_0": "Niyyātha candasūriyā,",
  "ja544:150.4_0": "Yaññatthāya ekarājassa.",
  "ja544:151.1_0": "Maṁsarasabhojanā nhāpakasunhāpitā,",
  "ja544:151.2_0": "Kuṇḍalino agalucandanavilittā;",
  "ja544:151.3_0": "Niyyātha candasūriyā,",
  "ja544:151.4_0": "Mātu katvā hadayasokaṁ.",
  "ja544:152.1_0": "Maṁsarasabhojanā nhāpakasunhāpitā,",
  "ja544:152.2_0": "Kuṇḍalino agalucandanavilittā;",
  "ja544:152.3_0": "Niyyātha candasūriyā,",
  "ja544:152.4_0": "Janassa katvā hadayasokaṁ”.",
  "ja544:153.1_0": "Sabbasmiṁ upakkhaṭasmiṁ,",
  "ja544:153.2_0": "Nisīdite candasmiṁ yaññatthāya;",
  "ja544:153.3_0": "Pañcālarājadhītā pañjalikā,",
  "ja544:153.4_0": "Sabbaparisāya samanupariyāyi.",
  "ja544:154.1_0": "“Yena saccena khaṇḍahālo,",
  "ja544:154.2_0": "Pāpakammaṁ karoti dummedho;",
  "ja544:154.3_0": "Etena saccavajjena,",
  "ja544:154.4_0": "Samaṅginī sāmikena homi.",
  "ja544:155.1_0": "Ye idhatthi amanussā,",
  "ja544:155.2_0": "Yāni ca yakkhabhūtabhabyāni;",
  "ja544:155.3_0": "Karontu me veyyāvaṭikaṁ,",
  "ja544:155.4_0": "Samaṅginī sāmikena homi.",
  "ja544:156.1_0": "Yā devatā idhāgatā,",
  "ja544:156.2_0": "Yāni ca yakkhabhūtabhabyāni;",
  "ja544:156.3_0": "Saraṇesiniṁ anāthaṁ tāyatha maṁ,",
  "ja544:156.4_0": "Yācāmahaṁ pati māhaṁ ajeyyaṁ”.",
  "ja544:157.1_0": "Taṁ sutvā amanusso,",
  "ja544:157.2_0": "Ayokūṭaṁ paribbhametvāna;",
  "ja544:157.3_0": "Bhayamassa janayanto,",
  "ja544:157.4_0": "Rājānaṁ idamavoca.",
  "ja544:158.1_0": "“Bujjhassu kho rājakali,",
  "ja544:158.2_0": "Mā tāhaṁ matthakaṁ nitāḷesiṁ;",
  "ja544:158.3_0": "Mā jeṭṭhaputtamavadhi,",
  "ja544:158.4_0": "Adūsakaṁ sīhasaṅkāsaṁ.",
  "ja544:159.1_0": "Ko te diṭṭho rājakali,",
  "ja544:159.2_0": "Puttabhariyāyo haññamānāyo;",
  "ja544:159.3_0": "Seṭṭhi ca gahapatayo,",
  "ja544:159.4_0": "Adūsakā saggakāmā hi”.",
  "ja544:160.1_0": "Taṁ sutvā khaṇḍahālo,",
  "ja544:160.2_0": "Rājā ca abbhutamidaṁ disvāna;",
  "ja544:160.3_0": "Sabbesaṁ bandhanāni mocesuṁ,",
  "ja544:160.4_0": "Yathā taṁ anupaghātaṁ.",
  "ja544:161.1_0": "Sabbesu vippamuttesu,",
  "ja544:161.2_0": "Ye tattha samāgatā tadā āsuṁ;",
  "ja544:161.3_0": "Sabbe ekekaleḍḍukamadaṁsu,",
  "ja544:161.4_0": "Esa vadho khaṇḍahālassa.",
  "ja544:162.1_0": "Sabbe paviṭṭhā nirayaṁ,",
  "ja544:162.2_0": "Yathā taṁ pāpakaṁ karitvāna;",
  "ja544:162.3_0": "Na hi pāpakammaṁ katvā,",
  "ja544:162.4_0": "Labbhā sugatiṁ ito gantuṁ.",
  "ja544:163.1_0": "Sabbesu vippamuttesu,",
  "ja544:163.2_0": "Ye tattha samāgatā tadā āsuṁ;",
  "ja544:163.3_0": "Candaṁ abhisiñciṁsu,",
  "ja544:163.4_0": "Samāgatā rājaparisā ca.",
  "ja544:164.1_0": "Sabbesu vippamuttesu,",
  "ja544:164.2_0": "Ye tattha samāgatā tadā āsuṁ;",
  "ja544:164.3_0": "Candaṁ abhisiñciṁsu,",
  "ja544:164.4_0": "Samāgatā rājakaññāyo ca.",
  "ja544:165.1_0": "Sabbesu vippamuttesu,",
  "ja544:165.2_0": "Ye tattha samāgatā tadā āsuṁ;",
  "ja544:165.3_0": "Candaṁ abhisiñciṁsu,",
  "ja544:165.4_0": "Samāgatā devaparisā ca.",
  "ja544:166.1_0": "Sabbesu vippamuttesu,",
  "ja544:166.2_0": "Ye tattha samāgatā tadā āsuṁ;",
  "ja544:166.3_0": "Candaṁ abhisiñciṁsu,",
  "ja544:166.4_0": "Samāgatā devakaññāyo ca.",
  "ja544:167.1_0": "Sabbesu vippamuttesu,",
  "ja544:167.2_0": "Ye tattha samāgatā tadā āsuṁ;",
  "ja544:167.3_0": "Celukkhepamakaruṁ,",
  "ja544:167.4_0": "Samāgatā rājaparisā ca.",
  "ja544:168.1_0": "Sabbesu vippamuttesu,",
  "ja544:168.2_0": "Ye tattha samāgatā tadā āsuṁ;",
  "ja544:168.3_0": "Celukkhepamakaruṁ,",
  "ja544:168.4_0": "Samāgatā rājakaññāyo ca.",
  "ja544:169.1_0": "Sabbesu vippamuttesu,",
  "ja544:169.2_0": "Ye tattha samāgatā tadā āsuṁ;",
  "ja544:169.3_0": "Celukkhepamakaruṁ,",
  "ja544:169.4_0": "Samāgatā devaparisā ca.",
  "ja544:170.1_0": "Sabbesu vippamuttesu,",
  "ja544:170.2_0": "Ye tattha samāgatā tadā āsuṁ;",
  "ja544:170.3_0": "Celukkhepamakaruṁ,",
  "ja544:170.4_0": "Samāgatā devakaññāyo ca.",
  "ja544:171.1_0": "Sabbesu vippamuttesu,",
  "ja544:171.2_0": "Bahū ānanditā ahuṁ;",
  "ja544:171.3_0": "Nandiṁ pavesi nagaraṁ,",
  "ja544:171.4_0": "Bandhanā mokkho aghositthāti.",
  "ja544:172.1_0": "Candakumārajātakaṁ sattamaṁ."
}