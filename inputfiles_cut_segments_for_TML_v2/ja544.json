{
  "ja544:0.1_0": "Jātaka",
  "ja544:0.2_0": "Mahānipāta",
  "ja544:0.3_0": "Mūgapakkhavagga",
  "ja544:0.4_0": "7 Candakumārajātaka",
  "ja544:1.1_0": "Rājāsi luddakammo,",
  "ja544:1.2_0": "Ekarājā pupphavatīyā;",
  "ja544:1.3_0": "So pucchi brahmabandhuṁ,",
  "ja544:1.4_0": "Khaṇḍahālaṁ purohitaṁ mūḷhaṁ.",
  "ja544:2.1_0": "“Saggāna maggamācikkha,",
  "ja544:2.2_0": "Tvaṁsi brāhmaṇa dhammavinayakusalo;",
  "ja544:2.3_0": "Yathā ito vajanti sugatiṁ,",
  "ja544:2.4_0": "Narā puññāni katvāna”.",
  "ja544:3.1_0": "“Atidānaṁ daditvāna,",
  "ja544:3.2_0": "avajjhe deva ghātetvā;",
  "ja544:3.3_0": "Evaṁ vajanti sugatiṁ,",
  "ja544:3.4_0": "narā puññāni katvāna”.",
  "ja544:4.1_0": "“Kiṁ pana taṁ atidānaṁ,",
  "ja544:4.2_0": "Ke ca avajjhā imasmi lokasmiṁ;",
  "ja544:4.3_0": "Etañca kho no akkhāhi,",
  "ja544:4.4_0": "Yajissāmi dadāmi dānāni”.",
  "ja544:5.1_0": "“Puttehi deva yajitabbaṁ,",
  "ja544:5.2_0": "Mahesīhi negamehi ca;",
  "ja544:5.3_0": "Usabhehi ājāniyehi catūhi,",
  "ja544:5.4_0": "Sabbacatukkena deva yajitabbaṁ”.",
  "ja544:6.1_0": "Taṁ sutvā antepure,",
  "ja544:6.2_0": "Kumārā mahesiyo ca haññantu;",
  "ja544:6.3_0": "Eko ahosi nigghoso,",
  "ja544:6.4_0": "Bhismā accuggato saddo.",
  "ja544:7.1_0": "“Gacchatha vadetha kumāre,",
  "ja544:7.2_0": "Candaṁ sūriyañca bhaddasenañca;",
  "ja544:7.3_0": "Sūrañca vāmagottañca,",
  "ja544:7.4_0": "Pacurā kira hotha yaññatthāya.",
  "ja544:8.1_0": "Kumāriyopi vadetha,",
  "ja544:8.2_0": "Upasenaṁ kokilañca muditañca;",
  "ja544:8.3_0": "Nandañcāpi kumāriṁ,",
  "ja544:8.4_0": "Pacurā kira hotha yaññatthāya.",
  "ja544:9.1_0": "Vijayampi mayhaṁ mahesiṁ,",
  "ja544:9.2_0": "Erāvatiṁ kesiniṁ sunandañca;",
  "ja544:9.3_0": "Lakkhaṇavarūpapannā,",
  "ja544:9.4_0": "Pacurā kira hotha yaññatthāya.",
  "ja544:10.1_0": "Gahapatayo ca vadetha,",
  "ja544:10.2_0": "Puṇṇamukhaṁ bhaddiyaṁ siṅgālañca;",
  "ja544:10.3_0": "Vaḍḍhañcāpi gahapatiṁ,",
  "ja544:10.4_0": "Pacurā kira hotha yaññatthāya”.",
  "ja544:11.1_0": "Te tattha gahapatayo,",
  "ja544:11.2_0": "Avocisuṁ samāgatā puttadāraparikiṇṇā;",
  "ja544:11.3_0": "“Sabbeva sikhino deva karohi,",
  "ja544:11.4_0": "Atha vā no dāse sāvehi”.",
  "ja544:12.1_0": "“Abhayaṅkarampi me hatthiṁ,",
  "ja544:12.2_0": "Nāḷāgiriṁ accuggataṁ varuṇadantaṁ;",
  "ja544:12.3_0": "Ānetha kho ne khippaṁ,",
  "ja544:12.4_0": "Yaññatthāya bhavissanti.",
  "ja544:13.1_0": "Assaratanampi kesiṁ,",
  "ja544:13.2_0": "Surāmukhaṁ puṇṇakaṁ vinatakañca;",
  "ja544:13.3_0": "Ānetha kho ne khippaṁ,",
  "ja544:13.4_0": "Yaññatthāya bhavissanti.",
  "ja544:14.1_0": "Usabhampi yūthapatiṁ anojaṁ,",
  "ja544:14.2_0": "Nisabhaṁ gavampatiṁ tepi mayhaṁ ānetha;",
  "ja544:14.3_0": "Samūha karontu sabbaṁ,",
  "ja544:14.4_0": "Yajissāmi dadāmi dānāni.",
  "ja544:15.1_0": "Sabbaṁ paṭiyādetha,",
  "ja544:15.2_0": "Yaññaṁ pana uggatamhi sūriyamhi;",
  "ja544:15.3_0": "Āṇāpetha ca kumāre,",
  "ja544:15.4_0": "Abhiramantu imaṁ rattiṁ.",
  "ja544:16.1_0": "Sabbaṁ upaṭṭhapetha,",
  "ja544:16.2_0": "Yaññaṁ pana uggatamhi sūriyamhi;",
  "ja544:16.3_0": "Vadetha dāni kumāre,",
  "ja544:16.4_0": "Ajja kho pacchimā ratti”.",
  "ja544:17.1_0": "Taṁtaṁ mātā avaca,",
  "ja544:17.2_0": "Rodantī āgantvā vimānato;",
  "ja544:17.3_0": "“Yañño kira te putta,",
  "ja544:17.4_0": "Bhavissati catūhi puttehi”.",
  "ja544:18.1_0": "“Sabbepi mayhaṁ puttā cattā,",
  "ja544:18.2_0": "candasmiṁ haññamānasmiṁ;",
  "ja544:18.3_0": "Puttehi yaññaṁ yajitvāna,",
  "ja544:18.4_0": "sugatiṁ saggaṁ gamissāmi”.",
  "ja544:19.1_0": "“Mā taṁ putta saddahesi,",
  "ja544:19.2_0": "Sugati kira hoti puttayaññena;",
  "ja544:19.3_0": "Nirayāneso maggo,",
  "ja544:19.4_0": "Neso maggo hi saggānaṁ.",
  "ja544:20.1_0": "Dānāni dehi koṇḍañña,",
  "ja544:20.2_0": "Ahiṁsā sabbabhūtabhabyānaṁ;",
  "ja544:20.3_0": "Esa maggo sugatiyā,",
  "ja544:20.4_0": "Na ca maggo puttayaññena”.",
  "ja544:21.1_0": "“Ācariyānaṁ vacanā,",
  "ja544:21.2_0": "Ghātessaṁ candañca sūriyañca;",
  "ja544:21.3_0": "Puttehi yaññaṁ yajitvāna duccajehi,",
  "ja544:21.4_0": "Sugatiṁ saggaṁ gamissāmi”.",
  "ja544:22.1_0": "Taṁtaṁ pitāpi avaca,",
  "ja544:22.2_0": "Vasavattī orasaṁ sakaṁ puttaṁ;",
  "ja544:22.3_0": "“Yañño kira te putta,",
  "ja544:22.4_0": "Bhavissati catūhi puttehi”.",
  "ja544:23.1_0": "“Sabbepi mayhaṁ puttā cattā,",
  "ja544:23.2_0": "candasmiṁ haññamānasmiṁ;",
  "ja544:23.3_0": "Puttehi yaññaṁ yajitvāna,",
  "ja544:23.4_0": "sugatiṁ saggaṁ gamissāmi”.",
  "ja544:24.1_0": "“Mā taṁ putta saddahesi,",
  "ja544:24.2_0": "Sugati kira hoti puttayaññena;",
  "ja544:24.3_0": "Nirayāneso maggo,",
  "ja544:24.4_0": "Neso maggo hi saggānaṁ.",
  "ja544:25.1_0": "Dānāni dehi koṇḍañña,",
  "ja544:25.2_0": "Ahiṁsā sabbabhūtabhabyānaṁ;",
  "ja544:25.3_0": "Esa maggo sugatiyā,",
  "ja544:25.4_0": "Na ca maggo puttayaññena”.",
  "ja544:26.1_0": "“Ācariyānaṁ vacanā,",
  "ja544:26.2_0": "Ghātessaṁ candañca sūriyañca;",
  "ja544:26.3_0": "Puttehi yaññaṁ yajitvāna duccajehi,",
  "ja544:26.4_0": "Sugatiṁ saggaṁ gamissāmi”.",
  "ja544:27.1_0": "“Dānāni dehi koṇḍañña,",
  "ja544:27.2_0": "Ahiṁsā sabbabhūtabhabyānaṁ;",
  "ja544:27.3_0": "Puttaparivuto tuvaṁ,",
  "ja544:27.4_0": "Raṭṭhaṁ janapadañca pālehi”.",
  "ja544:28.1_0": "“Mā no deva avadhi,",
  "ja544:28.2_0": "Dāse no dehi khaṇḍahālassa;",
  "ja544:28.3_0": "Api nigaḷabandhakāpi,",
  "ja544:28.4_0": "Hatthī asse ca pālema.",
  "ja544:29.1_0": "Mā no deva avadhi,",
  "ja544:29.2_0": "Dāse no dehi khaṇḍahālassa;",
  "ja544:29.3_0": "Api nigaḷabandhakāpi,",
  "ja544:29.4_0": "Hatthichakaṇāni ujjhema.",
  "ja544:30.1_0": "Mā no deva avadhi,",
  "ja544:30.2_0": "Dāse no dehi khaṇḍahālassa;",
  "ja544:30.3_0": "Api nigaḷabandhakāpi,",
  "ja544:30.4_0": "Assachakaṇāni ujjhema.",
  "ja544:31.1_0": "Mā no deva avadhi,",
  "ja544:31.2_0": "Dāse no dehi khaṇḍahālassa;",
  "ja544:31.3_0": "Yassa honti tava kāmā,",
  "ja544:31.4_0": "Api raṭṭhā pabbājitā;",
  "ja544:31.5_0": "Bhikkhācariyaṁ carissāma”.",
  "ja544:32.1_0": "“Dukkhaṁ kho me janayatha,",
  "ja544:32.2_0": "Vilapantā jīvitassa kāmā hi;",
  "ja544:32.3_0": "Muñcetha dāni kumāre,",
  "ja544:32.4_0": "Alampi me hotu puttayaññena”.",
  "ja544:33.1_0": "“Pubbeva khosi me vutto,",
  "ja544:33.2_0": "Dukkaraṁ durabhisambhavañcetaṁ;",
  "ja544:33.3_0": "Atha no upakkhaṭassa yaññassa,",
  "ja544:33.4_0": "Kasmā karosi vikkhepaṁ.",
  "ja544:34.1_0": "Sabbe vajanti sugatiṁ,",
  "ja544:34.2_0": "Ye yajanti yepi yājenti;",
  "ja544:34.3_0": "Ye cāpi anumodanti,",
  "ja544:34.4_0": "Yajantānaṁ edisaṁ mahāyaññaṁ”.",
  "ja544:35.1_0": "“Atha kissa jano pubbe,",
  "ja544:35.2_0": "Sotthānaṁ brāhmaṇe avācesi;",
  "ja544:35.3_0": "Atha no akāraṇasmā,",
  "ja544:35.4_0": "Yaññatthāya deva ghātesi.",
  "ja544:36.1_0": "Pubbeva no daharakāle,",
  "ja544:36.2_0": "na hanesi na ghātesi;",
  "ja544:36.3_0": "Daharamhā yobbanaṁ pattā,",
  "ja544:36.4_0": "adūsakā tāta haññāma.",
  "ja544:37.1_0": "Hatthigate assagate,",
  "ja544:37.2_0": "Sannaddhe passa no mahārāja;",
  "ja544:37.3_0": "Yuddhe vā yujjhamāne vā,",
  "ja544:37.4_0": "Na hi mādisā sūrā honti yaññatthāya.",
  "ja544:38.1_0": "Paccante vāpi kupite,",
  "ja544:38.2_0": "Aṭavīsu vā mādise niyojenti;",
  "ja544:38.3_0": "Atha no akāraṇasmā,",
  "ja544:38.4_0": "Abhūmiyaṁ tāta haññāma.",
  "ja544:39.1_0": "Yāpi hi tā sakuṇiyo,",
  "ja544:39.2_0": "Vasanti tiṇagharāni katvāna;",
  "ja544:39.3_0": "Tāsampi piyā puttā,",
  "ja544:39.4_0": "Atha no tvaṁ deva ghātesi.",
  "ja544:40.1_0": "Mā tassa saddahesi,",
  "ja544:40.2_0": "Na maṁ khaṇḍahālo ghāteyya;",
  "ja544:40.3_0": "Mamañhi so ghātetvāna,",
  "ja544:40.4_0": "Anantarā tampi deva ghāteyya.",
  "ja544:41.1_0": "Gāmavaraṁ nigamavaraṁ dadanti,",
  "ja544:41.2_0": "Bhogampissa mahārāja;",
  "ja544:41.3_0": "Athaggapiṇḍikāpi,",
  "ja544:41.4_0": "Kule kule hete bhuñjanti.",
  "ja544:42.1_0": "Tesampi tādisānaṁ,",
  "ja544:42.2_0": "Icchanti dubbhituṁ mahārāja;",
  "ja544:42.3_0": "Yebhuyyena ete,",
  "ja544:42.4_0": "Akataññuno brāhmaṇā deva.",
  "ja544:43.1_0": "Mā no deva avadhi,",
  "ja544:43.2_0": "Dāse no dehi khaṇḍahālassa;",
  "ja544:43.3_0": "Api nigaḷabandhakāpi,",
  "ja544:43.4_0": "Hatthī asse ca pālema.",
  "ja544:44.1_0": "Mā no deva avadhi,",
  "ja544:44.2_0": "Dāse no dehi khaṇḍahālassa;",
  "ja544:44.3_0": "Api nigaḷabandhakāpi,",
  "ja544:44.4_0": "Hatthichakaṇāni ujjhema.",
  "ja544:45.1_0": "Mā no deva avadhi,",
  "ja544:45.2_0": "Dāse no dehi khaṇḍahālassa;",
  "ja544:45.3_0": "Api nigaḷabandhakāpi,",
  "ja544:45.4_0": "Assachakaṇāni ujjhema.",
  "ja544:46.1_0": "Mā no deva avadhi,",
  "ja544:46.2_0": "Dāse no dehi khaṇḍahālassa;",
  "ja544:46.3_0": "Yassa honti tava kāmā,",
  "ja544:46.4_0": "Api raṭṭhā pabbājitā;",
  "ja544:46.5_0": "Bhikkhācariyaṁ carissāma”.",
  "ja544:47.1_0": "“Dukkhaṁ kho me janayatha,",
  "ja544:47.2_0": "Vilapantā jīvitassa kāmā hi;",
  "ja544:47.3_0": "Muñcetha dāni kumāre,",
  "ja544:47.4_0": "Alampi me hotu puttayaññena”.",
  "ja544:48.1_0": "“Pubbeva khosi me vutto,",
  "ja544:48.2_0": "Dukkaraṁ durabhisambhavañcetaṁ;",
  "ja544:48.3_0": "Atha no upakkhaṭassa yaññassa,",
  "ja544:48.4_0": "Kasmā karosi vikkhepaṁ.",
  "ja544:49.1_0": "Sabbe vajanti sugatiṁ,",
  "ja544:49.2_0": "Ye yajanti yepi yājenti;",
  "ja544:49.3_0": "Ye cāpi anumodanti,",
  "ja544:49.4_0": "Yajantānaṁ edisaṁ mahāyaññaṁ”.",
  "ja544:50.1_0": "“Yadi kira yajitvā puttehi,",
  "ja544:50.2_0": "Devalokaṁ ito cutā yanti;",
  "ja544:50.3_0": "Brāhmaṇo tāva yajatu,",
  "ja544:50.4_0": "Pacchāpi yajasi tuvaṁ rājā.",
  "ja544:51.1_0": "Yadi kira yajitvā puttehi,",
  "ja544:51.2_0": "Devalokaṁ ito cutā yanti;",
  "ja544:51.3_0": "Esveva khaṇḍahālo,",
  "ja544:51.4_0": "Yajataṁ sakehi puttehi.",
  "ja544:52.1_0": "Evaṁ jānanto khaṇḍahālo,",
  "ja544:52.2_0": "kiṁ puttake na ghātesi;",
  "ja544:52.3_0": "Sabbañca ñātijanaṁ,",
  "ja544:52.4_0": "attānañca na ghātesi.",
  "ja544:53.1_0": "Sabbe vajanti nirayaṁ,",
  "ja544:53.2_0": "Ye yajanti yepi yājenti;",
  "ja544:53.3_0": "Ye cāpi anumodanti,",
  "ja544:53.4_0": "Yajantānaṁ edisaṁ mahāyaññaṁ.",
  "ja544:54.1_0": "Sace hi so sujjhati yo hanāti,",
  "ja544:54.2_0": "Hatopi so saggamupeti ṭhānaṁ;",
  "ja544:54.3_0": "Bhovādi bhovādina mārayeyyuṁ,",
  "ja544:54.4_0": "Ye cāpi tesaṁ abhisaddaheyyuṁ.",
  "ja544:55.1_0": "Kathañca kira puttakāmāyo,",
  "ja544:55.2_0": "Gahapatayo gharaṇiyo ca;",
  "ja544:55.3_0": "Nagaramhi na uparavanti rājānaṁ,",
  "ja544:55.4_0": "‘Mā ghātayi orasaṁ puttaṁ’.",
  "ja544:56.1_0": "Kathañca kira puttakāmāyo,",
  "ja544:56.2_0": "Gahapatayo gharaṇiyo ca;",
  "ja544:56.3_0": "Nagaramhi na uparavanti rājānaṁ,",
  "ja544:56.4_0": "‘Mā ghātayi atrajaṁ puttaṁ’.",
  "ja544:57.1_0": "Rañño camhi atthakāmo,",
  "ja544:57.2_0": "Hito ca sabbajanapadassa;",
  "ja544:57.3_0": "Na koci assa paṭighaṁ,",
  "ja544:57.4_0": "Mayā jānapado na pavedeti.",
  "ja544:58.1_0": "Gacchatha vo gharaṇiyo,",
  "ja544:58.2_0": "Tātañca vadetha khaṇḍahālañca;",
  "ja544:58.3_0": "‘Mā ghātetha kumāre,",
  "ja544:58.4_0": "Adūsake sīhasaṅkāse’.",
  "ja544:59.1_0": "Gacchatha vo gharaṇiyo,",
  "ja544:59.2_0": "Tātañca vadetha khaṇḍahālañca;",
  "ja544:59.3_0": "‘Mā ghātetha kumāre,",
  "ja544:59.4_0": "Apekkhite sabbalokassa’.",
  "ja544:60.1_0": "Yannūnāhaṁ jāyeyyaṁ,",
  "ja544:60.2_0": "Rathakārakulesu vā;",
  "ja544:60.3_0": "Pukkusakulesu vā vessesu vā jāyeyyaṁ,",
  "ja544:60.4_0": "Na hajja maṁ rāja yaññe ghāteyya”.",
  "ja544:61.1_0": "“Sabbā sīmantiniyo gacchatha,",
  "ja544:61.2_0": "Ayyassa khaṇḍahālassa;",
  "ja544:61.3_0": "Pādesu nipatatha,",
  "ja544:61.4_0": "Aparādhāhaṁ na passāmi.",
  "ja544:62.1_0": "Sabbā sīmantiniyo gacchatha,",
  "ja544:62.2_0": "Ayyassa khaṇḍahālassa;",
  "ja544:62.3_0": "Pādesu nipatatha,",
  "ja544:62.4_0": "Kinte bhante mayaṁ adūsema”.",
  "ja544:63.1_0": "Kapaṇā vilapati selā,",
  "ja544:63.2_0": "Disvāna bhātare upanītatte;",
  "ja544:63.3_0": "“Yañño kira me ukkhipito,",
  "ja544:63.4_0": "Tātena saggakāmena”.",
  "ja544:64.1_0": "Āvatti parivatti ca,",
  "ja544:64.2_0": "vasulo sammukhā rañño;",
  "ja544:64.3_0": "“Mā no pitaraṁ avadhi,",
  "ja544:64.4_0": "daharamhāyobbanaṁ pattā”.",
  "ja544:65.1_0": "“Eso te vasula pitā,",
  "ja544:65.2_0": "Samehi pitarā saha;",
  "ja544:65.3_0": "Dukkhaṁ kho me janayasi,",
  "ja544:65.4_0": "Vilapanto antepurasmiṁ;",
  "ja544:65.5_0": "Muñcetha dāni kumāre,",
  "ja544:65.6_0": "Alampi me hotu puttayaññena”.",
  "ja544:66.1_0": "“Pubbeva khosi me vutto,",
  "ja544:66.2_0": "Dukkaraṁ durabhisambhavañcetaṁ;",
  "ja544:66.3_0": "Atha no upakkhaṭassa yaññassa,",
  "ja544:66.4_0": "Kasmā karosi vikkhepaṁ.",
  "ja544:67.1_0": "Sabbe vajanti sugatiṁ,",
  "ja544:67.2_0": "Ye yajanti yepi yājenti;",
  "ja544:67.3_0": "Ye cāpi anumodanti,",
  "ja544:67.4_0": "Yajantānaṁ edisaṁ mahāyaññaṁ.",
  "ja544:68.1_0": "Sabbaratanassa yañño upakkhaṭo,",
  "ja544:68.2_0": "Ekarāja tava paṭiyatto;",
  "ja544:68.3_0": "Abhinikkhamassu deva,",
  "ja544:68.4_0": "Saggaṁ gato tvaṁ pamodissasi”.",
  "ja544:69.1_0": "Daharā sattasatā etā,",
  "ja544:69.2_0": "Candakumārassa bhariyāyo;",
  "ja544:69.3_0": "Kese pakiritvāna,",
  "ja544:69.4_0": "Rodantiyo maggamanuyāyisuṁ.",
  "ja544:70.1_0": "Aparā pana sokena,",
  "ja544:70.2_0": "Nikkhantā nandane viya devā;",
  "ja544:70.3_0": "Kese pakiritvāna,",
  "ja544:70.4_0": "Rodantiyo maggamanuyāyiṁsu.",
  "ja544:71.1_0": "“Kāsikasucivatthadharā,",
