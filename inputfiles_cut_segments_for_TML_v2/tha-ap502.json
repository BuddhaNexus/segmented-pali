{
  "tha-ap502:0.1_0": "Therāpadāna",
  "tha-ap502:0.2_0": "Kiṅkaṇipupphavagga",
  "tha-ap502:0.3_0": "10. Piyālaphaladāyakattheraapadāna",
  "tha-ap502:1.1_0": "“Migaluddo pure āsiṁ,",
  "tha-ap502:1.2_0": "vipine vicaraṁ tadā;",
  "tha-ap502:1.3_0": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap502:1.4_0": "sabbadhammāna pāraguṁ.",
  "tha-ap502:2.1_0": "Piyālaphalamādāya,",
  "tha-ap502:2.2_0": "buddhaseṭṭhassadāsahaṁ;",
  "tha-ap502:2.3_0": "Puññakkhettassa vīrassa,",
  "tha-ap502:2.4_0": "pasanno sehi pāṇibhi.",
  "tha-ap502:3.1_0": "Ekatiṁse ito kappe,",
  "tha-ap502:3.2_0": "yaṁ phalaṁ adadiṁ tadā;",
  "tha-ap502:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap502:3.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap502:4.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap502:4.2_0": "…pe…",
  "tha-ap502:4.3_0": "viharāmi anāsavo.",
  "tha-ap502:5.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap502:5.2_0": "…pe…",
  "tha-ap502:5.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap502:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap502:6.2_0": "…pe…",
  "tha-ap502:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap502:7.1_0": "Itthaṁ sudaṁ āyasmā piyālaphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap502:8.1_0": "Piyālaphaladāyakattherassāpadānaṁ dasamaṁ.",
  "tha-ap502:8.2_0": "Kiṅkaṇipupphavaggo paññāsamo.",
  "tha-ap502:9.1_0": "Tassuddānaṁ",
  "tha-ap502:10.1_0": "Kiṅkaṇī paṁsukūlañca,",
  "tha-ap502:10.2_0": "koraṇḍamatha kiṁsukaṁ;",
  "tha-ap502:10.3_0": "Upaḍḍhadussī ghatado,",
  "tha-ap502:10.4_0": "udakaṁ thūpakārako.",
  "tha-ap502:11.1_0": "Naḷakārī ca navamo,",
  "tha-ap502:11.2_0": "piyālaphaladāyako;",
  "tha-ap502:11.3_0": "Satamekañca gāthānaṁ,",
  "tha-ap502:11.4_0": "navakañca taduttari.",
  "tha-ap502:12.1_0": "Atha vagguddānaṁ",
  "tha-ap502:13.1_0": "Metteyyavaggo bhaddāli,",
  "tha-ap502:13.2_0": "sakiṁsammajjakopi ca;",
  "tha-ap502:13.3_0": "Ekavihārī vibhītakī,",
  "tha-ap502:13.4_0": "jagatī sālapupphiyo.",
  "tha-ap502:14.1_0": "Naḷāgāraṁ paṁsukūlaṁ,",
  "tha-ap502:14.2_0": "kiṅkaṇipupphiyo tathā;",
  "tha-ap502:14.3_0": "Asīti dve ca gāthāyo,",
  "tha-ap502:14.4_0": "catuddasasatāni ca.",
  "tha-ap502:15.1_0": "Metteyyavaggadasakaṁ.",
  "tha-ap502:16.1_0": "Pañcamasatakaṁ samattaṁ."
}