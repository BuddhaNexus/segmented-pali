{
  "tha-ap351:0.1_0": "Therāpadāna",
  "tha-ap351:0.2_0": "Ekapadumiyavagga",
  "tha-ap351:0.3_0": "9. Tindukaphaladāyakattheraapadāna",
  "tha-ap351:1.1_0": "“Kaṇikāraṁva jotantaṁ,",
  "tha-ap351:1.2_0": "nisinnaṁ pabbatantare;",
  "tha-ap351:1.3_0": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap351:1.4_0": "oghatiṇṇamanāsavaṁ.",
  "tha-ap351:2.1_0": "Tindukaṁ saphalaṁ disvā,",
  "tha-ap351:2.2_0": "bhinditvāna sakosakaṁ;",
  "tha-ap351:2.3_0": "Pasannacitto sumano,",
  "tha-ap351:2.4_0": "sayambhussa madāsahaṁ.",
  "tha-ap351:3.1_0": "Ekanavutito kappe,",
  "tha-ap351:3.2_0": "yaṁ phalamadadiṁ tadā;",
  "tha-ap351:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap351:3.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap351:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap351:4.2_0": "…pe…",
  "tha-ap351:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap351:5.1_0": "Itthaṁ sudaṁ āyasmā tindukaphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap351:6.1_0": "Tindukaphaladāyakattherassāpadānaṁ navamaṁ."
}