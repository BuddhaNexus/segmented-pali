{
  "anya-e0605n11:0_0": "Apunarāvattigamanadīpanīyamakagāthā",
  "anya-e0605n11:1_0": "78.",
  "anya-e0605n11:2_0": "Anantakālopacitena tena,",
  "anya-e0605n11:3_0": "Puññena nibbattavimānayāne;",
  "anya-e0605n11:4_0": "Tasmiṁ dine jātasutaṁ pajāpatiṁ,",
  "anya-e0605n11:5_0": "Hitvā gato so sugato gato va.",
  "anya-e0605n11:6_0": "79.",
  "anya-e0605n11:7_0": "Taṁ jīvamānaṁ pitarañca mātaraṁ,",
  "anya-e0605n11:8_0": "Te ñātake tādisiyo ca itthiyo;",
  "anya-e0605n11:9_0": "Te tādise rammakare nikete,",
  "anya-e0605n11:10_0": "Hitvā gato so sugato gato va.",
  "anya-e0605n11:11_0": "80.",
  "anya-e0605n11:12_0": "Khomañca pattuṇṇadukūlacīnaṁ,",
  "anya-e0605n11:13_0": "Sakāsikaṁ sādhusugandhavāsitaṁ;",
  "anya-e0605n11:14_0": "Nivāsito sobhati vāsavo va,",
  "anya-e0605n11:15_0": "Hitvā gato so sugato gato va.",
  "anya-e0605n11:16_0": "81.",
  "anya-e0605n11:17_0": "Vidhippakāsā nidhiyo catasso,",
  "anya-e0605n11:18_0": "Samuggatā bhūtadharā vasundharā;",
  "anya-e0605n11:19_0": "Sattāvasattāvasudhā sudhāsā,",
  "anya-e0605n11:20_0": "Hitvā gato so sugato gato va.",
  "anya-e0605n11:21_0": "82.",
  "anya-e0605n11:22_0": "Suvaṇṇathāle satarājike subhe,",
  "anya-e0605n11:23_0": "Sādhuṁ sugandhaṁ sucisālibhojanaṁ;",
  "anya-e0605n11:24_0": "Bhutvā savāsīhi vilāsinīhi,",
  "anya-e0605n11:25_0": "Hitvā gata so sugato gato va.",
  "anya-e0605n11:26_0": "83.",
  "anya-e0605n11:27_0": "Manuññagandhena asuññagandho,",
  "anya-e0605n11:28_0": "Sugandhagandhena vilittagatto;",
  "anya-e0605n11:29_0": "Sugandhavātena suvijjitaṅgo,",
  "anya-e0605n11:30_0": "Hitvā gato so sugato gato va.",
  "anya-e0605n11:31_0": "84.",
  "anya-e0605n11:32_0": "Sulakkhaṇe hevabhilakkhitaṅgo,",
  "anya-e0605n11:33_0": "Pasādhito devapasādhanena;",
  "anya-e0605n11:34_0": "Virocamāno samarājinīhi,",
  "anya-e0605n11:35_0": "Hitvā gato so sugato gato va.",
  "anya-e0605n11:36_0": "85.",
  "anya-e0605n11:37_0": "Nānāsanāni sayanāni nivesanāni,",
  "anya-e0605n11:38_0": "Bhābhānibhāni ratanākarasannibhāni;",
  "anya-e0605n11:39_0": "Tatrussitāni ratanaddhajabhūsitāni,",
  "anya-e0605n11:40_0": "Hitvā va tāni himabindusamāni tāni.",
  "anya-e0605n11:41_0": "86.",
  "anya-e0605n11:42_0": "Nānāvidhehi ratanehi samujjalehi,",
  "anya-e0605n11:43_0": "Nārīhi niccamupagāyitahammiyehi;",
  "anya-e0605n11:44_0": "Rajjehi cakkaratanādivibhūsitehi,",
  "anya-e0605n11:45_0": "Yāto tato hi mahito purisassarehi."
}