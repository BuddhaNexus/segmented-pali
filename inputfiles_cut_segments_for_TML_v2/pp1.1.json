{
  "pp1.1:0.1_0": "Puggalapaññatti",
  "pp1.1:0.2_0": "Mātikā",
  "pp1.1:0.3_0": "1. Ekakauddesa",
  "pp1.1:1.1_0": "Namo tassa Bhagavato Arahato Sammāsambuddhassa.",
  "pp1.1:2.1_0": "Cha paññattiyo—",
  "pp1.1:2.2_0": "khandhapaññatti, āyatanapaññatti, dhātupaññatti, saccapaññatti, indriyapaññatti, puggalapaññattīti.",
  "pp1.1:3.1_0": "Kittāvatā khandhānaṁ khandhapaññatti?",
  "pp1.1:3.2_0": "Yāvatā pañcakkhandhā—",
  "pp1.1:3.3_0": "rūpakkhandho, vedanākkhandho, saññākkhandho, saṅkhārakkhandho, viññāṇakkhandho;",
  "pp1.1:3.4_0": "ettāvatā khandhānaṁ khandhapaññatti.",
  "pp1.1:4.1_0": "Kittāvatā āyatanānaṁ āyatanapaññatti?",
  "pp1.1:4.2_0": "Yāvatā dvādasāyatanāni—",
  "pp1.1:4.3_0": "cakkhāyatanaṁ, rūpāyatanaṁ, sotāyatanaṁ, saddāyatanaṁ, ghānāyatanaṁ, gandhāyatanaṁ, jivhāyatanaṁ, rasāyatanaṁ, kāyāyatanaṁ, phoṭṭhabbāyatanaṁ, manāyatanaṁ, dhammāyatanaṁ;",
  "pp1.1:4.4_0": "ettāvatā āyatanānaṁ āyatanapaññatti.",
  "pp1.1:5.1_0": "Kittāvatā dhātūnaṁ dhātupaññatti?",
  "pp1.1:5.2_0": "Yāvatā aṭṭhārasa dhātuyo—",
  "pp1.1:5.3_0": "cakkhudhātu, rūpadhātu, cakkhuviññāṇadhātu, sotadhātu, saddadhātu, sotaviññāṇadhātu, ghānadhātu, gandhadhātu, ghānaviññāṇadhātu, jivhādhātu, rasadhātu, jivhāviññāṇadhātu, kāyadhātu, phoṭṭhabbadhātu, kāyaviññāṇadhātu, manodhātu, dhammadhātu, manoviññāṇadhātu;",
  "pp1.1:5.4_0": "ettāvatā dhātūnaṁ dhātupaññatti.",
  "pp1.1:6.1_0": "Kittāvatā saccānaṁ saccapaññatti?",
  "pp1.1:6.2_0": "Yāvatā cattāri saccāni—",
  "pp1.1:6.3_0": "dukkhasaccaṁ, samudayasaccaṁ, nirodhasaccaṁ, maggasaccaṁ;",
  "pp1.1:6.4_0": "ettāvatā saccānaṁ saccapaññatti.",
  "pp1.1:7.1_0": "Kittāvatā indriyānaṁ indriyapaññatti?",
  "pp1.1:7.2_0": "Yāvatā bāvīsatindriyāni—",
  "pp1.1:7.3_0": "cakkhundriyaṁ, sotindriyaṁ, ghānindriyaṁ, jivhindriyaṁ, kāyindriyaṁ, manindriyaṁ, itthindriyaṁ, purisindriyaṁ, jīvitindriyaṁ, sukhindriyaṁ, dukkhindriyaṁ, somanassindriyaṁ, domanassindriyaṁ, upekkhindriyaṁ, saddhindriyaṁ, vīriyindriyaṁ, satindriyaṁ, samādhindriyaṁ, paññindriyaṁ, anaññātaññassāmītindriyaṁ, aññindriyaṁ, aññātāvindriyaṁ;",
  "pp1.1:7.4_0": "ettāvatā indriyānaṁ indriyapaññatti.",
  "pp1.1:8.1_0": "**Kittāvatā puggalānaṁ puggalapaññatti?**",
