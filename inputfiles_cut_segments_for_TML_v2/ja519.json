{
  "ja519:0.1_0": "Jātaka",
  "ja519:0.2_0": "Tiṁsanipāta",
  "ja519:0.3_0": "Kiṁchandavagga",
  "ja519:0.4_0": "9. Sambulājātaka",
  "ja519:1.1_0": "“Kā vedhamānā girikandarāyaṁ,",
  "ja519:1.2_0": "Ekā tuvaṁ tiṭṭhasi saṁhitūru;",
  "ja519:1.3_0": "Puṭṭhāsi me pāṇipameyyamajjhe,",
  "ja519:1.4_0": "Akkhāhi me nāmañca bandhave ca.",
  "ja519:2.1_0": "Obhāsayaṁ vanaṁ rammaṁ,",
  "ja519:2.2_0": "sīhabyagghanisevitaṁ;",
  "ja519:2.3_0": "Kā vā tvamasi kalyāṇi,",
  "ja519:2.4_0": "kassa vā tvaṁ sumajjhime;",
  "ja519:2.5_0": "Abhivādemi taṁ bhadde,",
  "ja519:2.6_0": "dānavāhaṁ namatthu te”.",
  "ja519:3.1_0": "“Yo putto kāsirājassa,",
  "ja519:3.2_0": "sotthisenoti taṁ vidū;",
  "ja519:3.3_0": "Tassāhaṁ sambulā bhariyā,",
  "ja519:3.4_0": "evaṁ jānāhi dānava;",
  "ja519:3.5_0": "Abhivādemi taṁ bhante,",
  "ja519:3.6_0": "sambulāhaṁ namatthu te.",
  "ja519:4.1_0": "Vedehaputto bhaddante,",
  "ja519:4.2_0": "vane vasati āturo;",
  "ja519:4.3_0": "Tamahaṁ rogasammattaṁ,",
  "ja519:4.4_0": "ekā ekaṁ upaṭṭhahaṁ.",
  "ja519:5.1_0": "Ahañca vanamuñchāya,",
  "ja519:5.2_0": "Madhumaṁsaṁ migābilaṁ;",
  "ja519:5.3_0": "Yadāharāmi taṁ bhakkho,",
  "ja519:5.4_0": "Tassa nūnajja nādhati”.",
  "ja519:6.1_0": "“Kiṁ vane rājaputtena,",
  "ja519:6.2_0": "āturena karissasi;",
  "ja519:6.3_0": "Sambule pariciṇṇena,",
  "ja519:6.4_0": "ahaṁ bhattā bhavāmi te”.",
  "ja519:7.1_0": "“Sokaṭṭāya durattāya,",
  "ja519:7.2_0": "kiṁ rūpaṁ vijjate mama;",
  "ja519:7.3_0": "Aññaṁ pariyesa bhaddante,",
  "ja519:7.4_0": "abhirūpataraṁ mayā”.",
  "ja519:8.1_0": "“Ehimaṁ girimāruyha,",
  "ja519:8.2_0": "bhariyā me catussatā;",
  "ja519:8.3_0": "Tāsaṁ tvaṁ pavarā hohi,",
  "ja519:8.4_0": "sabbakāmasamiddhinī.",
  "ja519:9.1_0": "Nūna tārakavaṇṇābhe,",
  "ja519:9.2_0": "yaṁ kiñci manasicchasi;",
  "ja519:9.3_0": "Sabbaṁ taṁ pacuraṁ mayhaṁ,",
  "ja519:9.4_0": "ramassvajja mayā saha.",
  "ja519:10.1_0": "No ce tuvaṁ maheseyyaṁ,",
  "ja519:10.2_0": "Sambule kārayissasi;",
  "ja519:10.3_0": "Alaṁ tvaṁ pātarāsāya,",
  "ja519:10.4_0": "Paṇhe bhakkhā bhavissasi”.",
  "ja519:11.1_0": "Tañca sattajaṭo luddo,",
  "ja519:11.2_0": "kaḷāro purisādako;",
  "ja519:11.3_0": "Vane nāthaṁ apassantiṁ,",
  "ja519:11.4_0": "sambulaṁ aggahī bhuje.",
  "ja519:12.1_0": "Adhipannā pisācena,",
  "ja519:12.2_0": "luddenāmisacakkhunā;",
  "ja519:12.3_0": "Sā ca sattuvasaṁ pattā,",
  "ja519:12.4_0": "patimevānusocati.",
  "ja519:13.1_0": "“Na me idaṁ tathā dukkhaṁ,",
  "ja519:13.2_0": "yaṁ maṁ khādeyya rakkhaso;",
  "ja519:13.3_0": "Yañca me ayyaputtassa,",
  "ja519:13.4_0": "mano hessati aññathā.",
  "ja519:14.1_0": "Na santi devā pavasanti nūna,",
  "ja519:14.2_0": "Na hi nūna santi idha lokapālā;",
  "ja519:14.3_0": "Sahasā karontānamasaññatānaṁ,",
  "ja519:14.4_0": "Na hi nūna santi paṭisedhitāro”.",
  "ja519:15.1_0": "“Itthīnamesā pavarā yasassinī,",
  "ja519:15.2_0": "Santā samā aggirivuggatejā;",
  "ja519:15.3_0": "Tañce tuvaṁ rakkhasādesi kaññaṁ,",
  "ja519:15.4_0": "Muddhā ca hi sattadhā te phaleyya;",
  "ja519:15.5_0": "Mā tvaṁ dahī muñca patibbatāya”.",
  "ja519:16.1_0": "Sā ca assamamāgacchi,",
  "ja519:16.2_0": "pamuttā purisādakā;",
  "ja519:16.3_0": "Nīḷaṁ paḷinaṁ sakuṇīva,",
  "ja519:16.4_0": "gatasiṅgaṁva ālayaṁ.",
  "ja519:17.1_0": "Sā tattha paridevesi,",
  "ja519:17.2_0": "rājaputtī yasassinī;",
  "ja519:17.3_0": "Sambulā utumattakkhā,",
  "ja519:17.4_0": "vane nāthaṁ apassantī.",
  "ja519:18.1_0": "Samaṇe brāhmaṇe vande,",
  "ja519:18.2_0": "sampannacaraṇe ise;",
  "ja519:18.3_0": "Rājaputtaṁ apassantī,",
  "ja519:18.4_0": "“tumhaṁmhi saraṇaṁ gatā”.",
  "ja519:19.1_0": "Vande sīhe ca byagghe ca,",
  "ja519:19.2_0": "ye ca aññe vane migā;",
  "ja519:19.3_0": "Rājaputtaṁ apassantī,",
  "ja519:19.4_0": "“tumhaṁmhi saraṇaṁ gatā”.",
  "ja519:20.1_0": "Tiṇā latāni osadhyo,",
  "ja519:20.2_0": "pabbatāni vanāni ca;",
  "ja519:20.3_0": "Rājaputtaṁ apassantī,",
  "ja519:20.4_0": "“tumhaṁmhi saraṇaṁ gatā”.",
  "ja519:21.1_0": "Vande indīvarīsāmaṁ,",
  "ja519:21.2_0": "rattiṁ nakkhattamāliniṁ;",
  "ja519:21.3_0": "Rājaputtaṁ apassantī,",
  "ja519:21.4_0": "“tumhaṁmhi saraṇaṁ gatā”.",
  "ja519:22.1_0": "Vande bhāgīrathiṁ gaṅgaṁ,",
  "ja519:22.2_0": "savantīnaṁ paṭiggahaṁ;",
  "ja519:22.3_0": "Rājaputtaṁ apassantī,",
  "ja519:22.4_0": "“tumhaṁmhi saraṇaṁ gatā”.",
  "ja519:23.1_0": "Vande ahaṁ pabbatarājaseṭṭhaṁ,",
  "ja519:23.2_0": "Himavantaṁ siluccayaṁ;",
  "ja519:23.3_0": "Rājaputtaṁ apassantī,",
  "ja519:23.4_0": "“Tumhaṁmhi saraṇaṁ gatā”.",
  "ja519:24.1_0": "“Atisāyaṁ vatāgacchi,",
  "ja519:24.2_0": "rājaputti yasassini;",
  "ja519:24.3_0": "Kena nujja samāgacchi,",
  "ja519:24.4_0": "ko te piyataro mayā”.",
  "ja519:25.1_0": "“Idaṁ khohaṁ tadāvocaṁ,",
  "ja519:25.2_0": "gahitā tena sattunā;",
  "ja519:25.3_0": "Na me idaṁ tathā dukkhaṁ,",
  "ja519:25.4_0": "yaṁ maṁ khādeyya rakkhaso;",
  "ja519:25.5_0": "Yañca me ayyaputtassa,",
  "ja519:25.6_0": "mano hessati aññathā”.",
  "ja519:26.1_0": "“Corīnaṁ bahubuddhīnaṁ,",
  "ja519:26.2_0": "yāsu saccaṁ sudullabhaṁ;",
  "ja519:26.3_0": "Thīnaṁ bhāvo durājāno,",
  "ja519:26.4_0": "macchassevodake gataṁ”.",
  "ja519:27.1_0": "“Tathā maṁ saccaṁ pāletu,",
  "ja519:27.2_0": "pālayissati ce mamaṁ;",
  "ja519:27.3_0": "Yathāhaṁ nābhijānāmi,",
  "ja519:27.4_0": "aññaṁ piyataraṁ tayā;",
  "ja519:27.5_0": "Etena saccavajjena,",
  "ja519:27.6_0": "byādhi te vūpasammatu”.",
  "ja519:28.1_0": "“Ye kuñjarā sattasatā uḷārā,",
  "ja519:28.2_0": "Rakkhanti rattindivamuyyutāvudhā;",
  "ja519:28.3_0": "Dhanuggahānañca satāni soḷasa,",
  "ja519:28.4_0": "Kathaṁvidhe passasi bhadde sattavo”.",
  "ja519:29.1_0": "“Alaṅkatāyo padumuttarattacā,",
  "ja519:29.2_0": "Virāgitā passati haṁsagaggarā;",
  "ja519:29.3_0": "Tāsaṁ suṇitvā mitagītavāditaṁ,",
  "ja519:29.4_0": "Na dāni me tāta tathā yathā pure.",
  "ja519:30.1_0": "Suvaṇṇasaṅkaccadharā suviggahā,",
  "ja519:30.2_0": "Alaṅkatā mānusiyaccharūpamā;",
  "ja519:30.3_0": "Senopiyā tāta aninditaṅgiyo,",
  "ja519:30.4_0": "Khattiyakaññā paṭilobhayanti naṁ.",
  "ja519:31.1_0": "Sace ahaṁ tāta tathā yathā pure,",
  "ja519:31.2_0": "Patiṁ tamuñchāya punā vane bhare;",
  "ja519:31.3_0": "Sammānaye maṁ na ca maṁ vimānaye,",
  "ja519:31.4_0": "Itopi me tāta tato varaṁ siyā.",
  "ja519:32.1_0": "Yamannapāne vipulasmi ohite,",
  "ja519:32.2_0": "Nārī vimaṭṭhābharaṇā alaṅkatā;",
  "ja519:32.3_0": "Sabbaṅgupetā patino ca appiyā,",
  "ja519:32.4_0": "Abajjha tassā maraṇaṁ tato varaṁ.",
  "ja519:33.1_0": "Api ce daliddā kapaṇā anāḷhiyā,",
  "ja519:33.2_0": "Kaṭādutīyā patino ca sā piyā;",
  "ja519:33.3_0": "Sabbaṅgupetāyapi appiyāya,",
  "ja519:33.4_0": "Ayameva seyyā kapaṇāpi yā piyā”.",
  "ja519:34.1_0": "“Sudullabhitthī purisassa yā hitā,",
  "ja519:34.2_0": "Bhattitthiyā dullabho yo hito ca;",
  "ja519:34.3_0": "Hitā ca te sīlavatī ca bhariyā,",
  "ja519:34.4_0": "Janinda dhammaṁ cara sambulāya”.",
  "ja519:35.1_0": "“Sace tuvaṁ vipule laddhabhoge,",
  "ja519:35.2_0": "Issāvatiṇṇā maraṇaṁ upesi;",
  "ja519:35.3_0": "Ahañca te bhadde imā rājakaññā,",
  "ja519:35.4_0": "Sabbe te vacanakarā bhavāmā”ti.",
  "ja519:36.1_0": "Sambulājātakaṁ navamaṁ."
}