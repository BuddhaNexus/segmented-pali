{
  "tha-ap563:0.1_0": "Therāpadāna",
  "tha-ap563:0.2_0": "Yasavagga",
  "tha-ap563:0.3_0": "11. Raṭṭhapālattheraapadāna",
  "tha-ap563:1.1_0": "“Padumuttarassa bhagavato,",
  "tha-ap563:1.2_0": "Lokajeṭṭhassa tādino;",
  "tha-ap563:1.3_0": "Varanāgo mayā dinno,",
  "tha-ap563:1.4_0": "Īsādanto urūḷhavā.",
  "tha-ap563:2.1_0": "Setacchattopasobhito,",
  "tha-ap563:2.2_0": "sakappano sahatthipo;",
  "tha-ap563:2.3_0": "Agghāpetvāna taṁ sabbaṁ,",
  "tha-ap563:2.4_0": "saṅghārāmaṁ akārayiṁ.",
  "tha-ap563:3.1_0": "Catupaññāsasahassāni,",
  "tha-ap563:3.2_0": "pāsāde kārayiṁ ahaṁ;",
  "tha-ap563:3.3_0": "Mahoghadānaṁ karitvāna,",
  "tha-ap563:3.4_0": "niyyādesiṁ mahesino.",
  "tha-ap563:4.1_0": "Anumodi mahāvīro,",
  "tha-ap563:4.2_0": "sayambhū aggapuggalo;",
  "tha-ap563:4.3_0": "Sabbe jane hāsayanto,",
  "tha-ap563:4.4_0": "desesi amataṁ padaṁ.",
  "tha-ap563:5.1_0": "Taṁ me buddho viyākāsi,",
  "tha-ap563:5.2_0": "jalajuttamanāmako;",
  "tha-ap563:5.3_0": "Bhikkhusaṅghe nisīditvā,",
  "tha-ap563:5.4_0": "imā gāthā abhāsatha.",
  "tha-ap563:6.1_0": "‘Catupaññāsasahassāni,",
  "tha-ap563:6.2_0": "pāsāde kārayī ayaṁ;",
  "tha-ap563:6.3_0": "Kathayissāmi vipākaṁ,",
  "tha-ap563:6.4_0": "suṇātha mama bhāsato.",
  "tha-ap563:7.1_0": "Aṭṭhārasasahassāni,",
  "tha-ap563:7.2_0": "kūṭāgārā bhavissare;",
  "tha-ap563:7.3_0": "Byamhuttamamhi nibbattā,",
  "tha-ap563:7.4_0": "sabbasoṇṇamayā ca te.",
  "tha-ap563:8.1_0": "Paññāsakkhattuṁ devindo,",
  "tha-ap563:8.2_0": "devarajjaṁ karissati;",
  "tha-ap563:8.3_0": "Aṭṭhapaññāsakkhattuñca,",
  "tha-ap563:8.4_0": "cakkavattī bhavissati.",
  "tha-ap563:9.1_0": "Kappasatasahassamhi,",
  "tha-ap563:9.2_0": "okkākakulasambhavo;",
  "tha-ap563:9.3_0": "Gotamo nāma gottena,",
  "tha-ap563:9.4_0": "satthā loke bhavissati.",
  "tha-ap563:10.1_0": "Devalokā cavitvāna,",
  "tha-ap563:10.2_0": "sukkamūlena codito;",
  "tha-ap563:10.3_0": "Aḍḍhe kule mahābhoge,",
  "tha-ap563:10.4_0": "nibbattissati tāvade.",
  "tha-ap563:11.1_0": "So pacchā pabbajitvāna,",
  "tha-ap563:11.2_0": "sukkamūlena codito;",
  "tha-ap563:11.3_0": "Raṭṭhapāloti nāmena,",
  "tha-ap563:11.4_0": "hessati satthu sāvako.",
  "tha-ap563:12.1_0": "Padhānapahitatto so,",
  "tha-ap563:12.2_0": "upasanto nirūpadhi;",
  "tha-ap563:12.3_0": "Sabbāsave pariññāya,",
  "tha-ap563:12.4_0": "nibbāyissatināsavo’.",
  "tha-ap563:13.1_0": "Uṭṭhāya abhinikkhamma,",
  "tha-ap563:13.2_0": "jahitā bhogasampadā;",
  "tha-ap563:13.3_0": "Kheḷapiṇḍeva bhogamhi,",
  "tha-ap563:13.4_0": "pemaṁ mayhaṁ na vijjati.",
  "tha-ap563:14.1_0": "Vīriyaṁ me dhuradhorayhaṁ,",
  "tha-ap563:14.2_0": "yogakkhemādhivāhanaṁ;",
  "tha-ap563:14.3_0": "Dhāremi antimaṁ dehaṁ,",
  "tha-ap563:14.4_0": "sammāsambuddhasāsane.",
  "tha-ap563:15.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap563:15.2_0": "…pe…",
  "tha-ap563:15.3_0": "viharāmi anāsavo.",
  "tha-ap563:16.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap563:16.2_0": "…pe…",
  "tha-ap563:16.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap563:17.1_0": "Paṭisambhidā catasso,",
  "tha-ap563:17.2_0": "…pe…",
  "tha-ap563:17.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap563:18.1_0": "Itthaṁ sudaṁ āyasmā raṭṭhapālo thero imā gāthāyo abhāsitthāti.",
  "tha-ap563:19.1_0": "Raṭṭhapālattherassāpadānaṁ ekādasamaṁ.",
  "tha-ap563:20.1_0": "Yasavaggo chapaññāsamo.",
  "tha-ap563:21.1_0": "Tassuddānaṁ",
  "tha-ap563:22.1_0": "Yaso nadīkassapo ca,",
  "tha-ap563:22.2_0": "gayākimilavajjino;",
  "tha-ap563:22.3_0": "Duve uttarā bhaddajī,",
  "tha-ap563:22.4_0": "sivako upavāhano;",
  "tha-ap563:22.5_0": "Raṭṭhapālo ekasataṁ,",
  "tha-ap563:22.6_0": "gāthānaṁ pañcanavuti.",
  "tha-ap563:23.1_0": "Therāpadānaṁ samattaṁ."
}