{
  "thag1.86:0.1_0": "Theragāthā",
  "thag1.86:0.2_0": "Ekakanipāta",
  "thag1.86:0.3_0": "Navamavagga",
  "thag1.86:0.4_0": "1.86. Nāgitattheragāthā",
  "thag1.86:1.1_0": "“Ito bahiddhā puthu aññavādinaṁ,",
  "thag1.86:1.2_0": "Maggo na nibbānagamo yathā ayaṁ;",
  "thag1.86:1.3_0": "Itissu saṅghaṁ bhagavānusāsati,",
  "thag1.86:1.4_0": "Satthā sayaṁ pāṇitaleva dassayan”ti.",
  "thag1.86:1.5_0": "… Nāgito thero …."
}