{
  "thag1.34:0.1_0": "Theragāthā",
  "thag1.34:0.2_0": "Ekakanipāta",
  "thag1.34:0.3_0": "Catutthavagga",
  "thag1.34:0.4_0": "1.34. Posiyattheragāthā",
  "thag1.34:1.1_0": "“Anāsannavarā etā,",
  "thag1.34:1.2_0": "niccameva vijānatā;",
  "thag1.34:1.3_0": "Gāmā araññamāgamma,",
  "thag1.34:1.4_0": "tato gehaṁ upāvisi;",
  "thag1.34:1.5_0": "Tato uṭṭhāya pakkāmi,",
  "thag1.34:1.6_0": "anāmantetvā posiyo”ti.",
  "thag1.34:1.7_0": "… Posiyo thero …."
}