{
  "thi-ap19:0.1_0": "Therīapadāna",
  "thi-ap19:0.2_0": "Ekūposathikavagga",
  "thi-ap19:0.3_0": "9. Uppalavaṇṇātherīapadāna",
  "thi-ap19:1.1_0": "“Bhikkhunī uppalavaṇṇā,",
  "thi-ap19:1.2_0": "iddhiyā pāramiṁ gatā;",
  "thi-ap19:1.3_0": "Vanditvā satthuno pāde,",
  "thi-ap19:1.4_0": "idaṁ vacanamabravi.",
  "thi-ap19:2.1_0": "‘Nitthiṇṇā jātisaṁsāraṁ,",
  "thi-ap19:2.2_0": "pattāhaṁ acalaṁ padaṁ;",
  "thi-ap19:2.3_0": "Sabbadukkhaṁ mayā khīṇaṁ,",
  "thi-ap19:2.4_0": "ārocemi mahāmuni.",
  "thi-ap19:3.1_0": "Yāvatā parisā atthi,",
  "thi-ap19:3.2_0": "pasannā jinasāsane;",
  "thi-ap19:3.3_0": "Yassā ca meparādhotthi,",
  "thi-ap19:3.4_0": "khamantu jinasammukhā.",
  "thi-ap19:4.1_0": "Saṁsāre saṁsarantiyā,",
  "thi-ap19:4.2_0": "khalitaṁ me sace bhave;",
  "thi-ap19:4.3_0": "Ārocemi mahāvīra,",
  "thi-ap19:4.4_0": "aparādhaṁ khamassu taṁ’.",
  "thi-ap19:5.1_0": "‘Iddhiñcāpi nidassehi,",
  "thi-ap19:5.2_0": "mama sāsanakārike;",
  "thi-ap19:5.3_0": "Catasso parisā ajja,",
  "thi-ap19:5.4_0": "kaṅkhaṁ chindāhi yāvatā’.",
  "thi-ap19:6.1_0": "‘Dhītā tuyhaṁ mahāvīra,",
  "thi-ap19:6.2_0": "paññavanta jutindhara;",
  "thi-ap19:6.3_0": "Bahuñca dukkaraṁ kammaṁ,",
  "thi-ap19:6.4_0": "kataṁ me atidukkaraṁ.",
  "thi-ap19:7.1_0": "Uppalasseva me vaṇṇo,",
  "thi-ap19:7.2_0": "nāmenuppalanāmikā;",
  "thi-ap19:7.3_0": "Sāvikā te mahāvīra,",
  "thi-ap19:7.4_0": "pāde vandāmi cakkhuma.",
  "thi-ap19:8.1_0": "Rāhulo ca ahañceva,",
  "thi-ap19:8.2_0": "nekajātisate bahū;",
  "thi-ap19:8.3_0": "Ekasmiṁ sambhave jātā,",
  "thi-ap19:8.4_0": "samānachandamānasā.",
  "thi-ap19:9.1_0": "Nibbatti ekato hoti,",
  "thi-ap19:9.2_0": "jātiyāpi ca ekato;",
  "thi-ap19:9.3_0": "Pacchime bhave sampatte,",
  "thi-ap19:9.4_0": "ubhopi nānāsambhavā.",
  "thi-ap19:10.1_0": "Putto ca rāhulo nāma,",
  "thi-ap19:10.2_0": "dhītā uppalasavhayā;",
  "thi-ap19:10.3_0": "Passa vīra mamaṁ iddhiṁ,",
  "thi-ap19:10.4_0": "balaṁ dassemi satthuno’.",
  "thi-ap19:11.1_0": "Mahāsamudde caturo,",
  "thi-ap19:11.2_0": "pakkhipi hatthapātiyaṁ;",
  "thi-ap19:11.3_0": "Telaṁ hatthagatañceva,",
  "thi-ap19:11.4_0": "khiḍḍo komārako yathā.",
  "thi-ap19:12.1_0": "Ubbattayitvā pathaviṁ,",
  "thi-ap19:12.2_0": "pakkhipi hatthapātiyaṁ;",
  "thi-ap19:12.3_0": "Cittaṁ muñjaṁ yathā nāma,",
  "thi-ap19:12.4_0": "luñci komārako yuvā.",
  "thi-ap19:13.1_0": "Cakkavāḷasamaṁ pāṇiṁ,",
  "thi-ap19:13.2_0": "chādayitvāna matthake;",
  "thi-ap19:13.3_0": "Vassāpetvāna phusitaṁ,",
  "thi-ap19:13.4_0": "nānāvaṇṇaṁ punappunaṁ.",
  "thi-ap19:14.1_0": "Bhūmiṁ udukkhalaṁ katvā,",
  "thi-ap19:14.2_0": "dhaññaṁ katvāna sakkharaṁ;",
  "thi-ap19:14.3_0": "Sineruṁ musalaṁ katvā,",
  "thi-ap19:14.4_0": "maddi komārikā yathā.",
  "thi-ap19:15.1_0": "‘Dhītāhaṁ buddhaseṭṭhassa,",
  "thi-ap19:15.2_0": "nāmenuppalasavhayā;",
  "thi-ap19:15.3_0": "Abhiññāsu vasībhūtā,",
  "thi-ap19:15.4_0": "tava sāsanakārikā.",
  "thi-ap19:16.1_0": "Nānāvikubbanaṁ katvā,",
  "thi-ap19:16.2_0": "dassetvā lokanāyakaṁ;",
  "thi-ap19:16.3_0": "Nāmagottañca sāvetvā,",
  "thi-ap19:16.4_0": "pāde vandāmi cakkhuma.",
  "thi-ap19:17.1_0": "Iddhīsu ca vasī homi,",
  "thi-ap19:17.2_0": "dibbāya sotadhātuyā;",
  "thi-ap19:17.3_0": "Cetopariyañāṇassa,",
  "thi-ap19:17.4_0": "vasī homi mahāmune.",
  "thi-ap19:18.1_0": "Pubbenivāsaṁ jānāmi,",
  "thi-ap19:18.2_0": "dibbacakkhu visodhitaṁ;",
  "thi-ap19:18.3_0": "Sabbāsavā parikkhīṇā,",
  "thi-ap19:18.4_0": "natthi dāni punabbhavo.",
  "thi-ap19:19.1_0": "Atthadhammaniruttīsu,",
  "thi-ap19:19.2_0": "Paṭibhāne tatheva ca;",
  "thi-ap19:19.3_0": "Ñāṇaṁ me vimalaṁ suddhaṁ,",
  "thi-ap19:19.4_0": "Sabhāvena mahesino.",
  "thi-ap19:20.1_0": "Purimānaṁ jinaggānaṁ,",
  "thi-ap19:20.2_0": "saṅgamaṁ te nidassitaṁ;",
  "thi-ap19:20.3_0": "Adhikāraṁ bahuṁ mayhaṁ,",
  "thi-ap19:20.4_0": "tuyhatthāya mahāmuni.",
  "thi-ap19:21.1_0": "Yaṁ mayā pūritaṁ kammaṁ,",
  "thi-ap19:21.2_0": "Kusalaṁ sara me muni;",
  "thi-ap19:21.3_0": "Tavatthāya mahāvīra,",
  "thi-ap19:21.4_0": "Puññaṁ upacitaṁ mayā.",
  "thi-ap19:22.1_0": "Abhabbaṭṭhāne vajjetvā,",
  "thi-ap19:22.2_0": "vārayantī anācaraṁ;",
  "thi-ap19:22.3_0": "Tavatthāya mahāvīra,",
  "thi-ap19:22.4_0": "cattaṁ me jīvituttamaṁ.",
  "thi-ap19:23.1_0": "Dasakoṭisahassāni,",
  "thi-ap19:23.2_0": "adāsiṁ mama jīvitaṁ;",
  "thi-ap19:23.3_0": "Pariccattā ca me homi,",
  "thi-ap19:23.4_0": "tavatthāya mahāmuni’.",
  "thi-ap19:24.1_0": "Tadātivimhitā sabbā,",
  "thi-ap19:24.2_0": "sirasāva katañjalī;",
  "thi-ap19:24.3_0": "‘Avocayye kathaṁ āsi,",
  "thi-ap19:24.4_0": "atuliddhiparakkamā’.",
  "thi-ap19:25.1_0": "Satasahassito kappe,",
  "thi-ap19:25.2_0": "nāgakaññā ahaṁ tadā;",
  "thi-ap19:25.3_0": "Vimalā nāma nāmena,",
  "thi-ap19:25.4_0": "kaññānaṁ sādhusammatā.",
  "thi-ap19:26.1_0": "Mahorago mahānāgo,",
  "thi-ap19:26.2_0": "pasanno jinasāsane;",
  "thi-ap19:26.3_0": "Padumuttaraṁ mahātejaṁ,",
  "thi-ap19:26.4_0": "nimantesi sasāvakaṁ.",
  "thi-ap19:27.1_0": "Ratanamayaṁ maṇḍapaṁ,",
  "thi-ap19:27.2_0": "pallaṅkaṁ ratanāmayaṁ;",
  "thi-ap19:27.3_0": "Ratanaṁ vālukākiṇṇaṁ,",
  "thi-ap19:27.4_0": "upabhogaṁ ratanāmayaṁ.",
  "thi-ap19:28.1_0": "Maggañca paṭiyādesi,",
  "thi-ap19:28.2_0": "ratanaddhajabhūsitaṁ;",
  "thi-ap19:28.3_0": "Paccuggantvāna sambuddhaṁ,",
  "thi-ap19:28.4_0": "vajjanto tūriyehi so.",
  "thi-ap19:29.1_0": "Parisāhi ca catūhi,",
  "thi-ap19:29.2_0": "parivuto lokanāyako;",
  "thi-ap19:29.3_0": "Mahoragassa bhavane,",
  "thi-ap19:29.4_0": "nisīdi paramāsane.",
  "thi-ap19:30.1_0": "Annaṁ pānaṁ khādanīyaṁ,",
  "thi-ap19:30.2_0": "bhojanañca mahārahaṁ;",
  "thi-ap19:30.3_0": "Varaṁ varañca pādāsi,",
  "thi-ap19:30.4_0": "nāgarājā mahāyasaṁ.",
  "thi-ap19:31.1_0": "Bhuñjitvāna sambuddho,",
  "thi-ap19:31.2_0": "pattaṁ dhovitvā yoniso;",
  "thi-ap19:31.3_0": "Anumodanīyaṅkāsi,",
  "thi-ap19:31.4_0": "nāgakaññā mahiddhikā.",
  "thi-ap19:32.1_0": "Sabbaññuṁ phullitaṁ disvā,",
  "thi-ap19:32.2_0": "nāgakaññā mahāyasaṁ;",
  "thi-ap19:32.3_0": "Pasannaṁ satthuno cittaṁ,",
  "thi-ap19:32.4_0": "sunibandhañca mānasaṁ.",
  "thi-ap19:33.1_0": "Mamañca cittamaññāya,",
  "thi-ap19:33.2_0": "jalajuttamanāmako;",
  "thi-ap19:33.3_0": "Tasmiṁ khaṇe mahāvīro,",
  "thi-ap19:33.4_0": "bhikkhuniṁ dassayiddhiyā.",
  "thi-ap19:34.1_0": "Iddhī anekā dassesi,",
  "thi-ap19:34.2_0": "bhikkhunī sā visāradā;",
  "thi-ap19:34.3_0": "Pamoditā vedajātā,",
  "thi-ap19:34.4_0": "satthāraṁ idamabravi.",
  "thi-ap19:35.1_0": "‘Addasāhaṁ imaṁ iddhiṁ,",
  "thi-ap19:35.2_0": "sumanaṁ itarāyapi;",
  "thi-ap19:35.3_0": "Kathaṁ ahosi sā vīra,",
  "thi-ap19:35.4_0": "iddhiyā suvisāradā’.",
  "thi-ap19:36.1_0": "‘Orasā mukhato jātā,",
  "thi-ap19:36.2_0": "dhītā mama mahiddhikā;",
  "thi-ap19:36.3_0": "Mamānusāsanikarā,",
  "thi-ap19:36.4_0": "iddhiyā suvisāradā’.",
  "thi-ap19:37.1_0": "Buddhassa vacanaṁ sutvā,",
  "thi-ap19:37.2_0": "evaṁ patthesahaṁ tadā;",
  "thi-ap19:37.3_0": "‘Ahampi tādisā homi,",
  "thi-ap19:37.4_0": "iddhiyā suvisāradā.",
  "thi-ap19:38.1_0": "Pamoditāhaṁ sumanā,",
  "thi-ap19:38.2_0": "patthe uttamamānasā;",
  "thi-ap19:38.3_0": "Anāgatamhi addhāne,",
  "thi-ap19:38.4_0": "īdisā homi nāyaka’.",
  "thi-ap19:39.1_0": "Maṇimayamhi pallaṅke,",
  "thi-ap19:39.2_0": "maṇḍapamhi pabhassare;",
  "thi-ap19:39.3_0": "Annapānena tappetvā,",
  "thi-ap19:39.4_0": "sasaṅghaṁ lokanāyakaṁ.",
  "thi-ap19:40.1_0": "Nāgānaṁ pavaraṁ pupphaṁ,",
  "thi-ap19:40.2_0": "aruṇaṁ nāma uppalaṁ;",
  "thi-ap19:40.3_0": "‘Vaṇṇaṁ me īdisaṁ hotu’,",
  "thi-ap19:40.4_0": "pūjesiṁ lokanāyakaṁ.",
  "thi-ap19:41.1_0": "Tena kammena sukatena,",
  "thi-ap19:41.2_0": "cetanāpaṇidhīhi ca;",
  "thi-ap19:41.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "thi-ap19:41.4_0": "tāvatiṁsamagacchahaṁ.",
  "thi-ap19:42.1_0": "Tato cutāhaṁ manuje,",
  "thi-ap19:42.2_0": "upapannā sayambhuno;",
  "thi-ap19:42.3_0": "Uppalehi paṭicchannaṁ,",
  "thi-ap19:42.4_0": "piṇḍapātamadāsahaṁ.",
  "thi-ap19:43.1_0": "Ekanavutito kappe,",
  "thi-ap19:43.2_0": "vipassī nāma nāyako;",
  "thi-ap19:43.3_0": "Uppajji cārudassano,",
  "thi-ap19:43.4_0": "sabbadhammesu cakkhumā.",
  "thi-ap19:44.1_0": "Seṭṭhidhītā tadā hutvā,",
  "thi-ap19:44.2_0": "bārāṇasipuruttame;",
  "thi-ap19:44.3_0": "Nimantetvāna sambuddhaṁ,",
  "thi-ap19:44.4_0": "sasaṅghaṁ lokanāyakaṁ.",
  "thi-ap19:45.1_0": "Mahādānaṁ daditvāna,",
  "thi-ap19:45.2_0": "uppalehi vināyakaṁ;",
  "thi-ap19:45.3_0": "Pūjayitvā cetasāva,",
  "thi-ap19:45.4_0": "vaṇṇasobhaṁ apatthayiṁ.",
  "thi-ap19:46.1_0": "Imamhi bhaddake kappe,",
  "thi-ap19:46.2_0": "brahmabandhu mahāyaso;",
  "thi-ap19:46.3_0": "Kassapo nāma gottena,",
  "thi-ap19:46.4_0": "uppajji vadataṁ varo.",
  "thi-ap19:47.1_0": "Upaṭṭhāko mahesissa,",
  "thi-ap19:47.2_0": "tadā āsi narissaro;",
  "thi-ap19:47.3_0": "Kāsirājā kikī nāma,",
  "thi-ap19:47.4_0": "bārāṇasipuruttame.",
  "thi-ap19:48.1_0": "Tassāsiṁ dutiyā dhītā,",
  "thi-ap19:48.2_0": "samaṇaguttasavhayā;",
  "thi-ap19:48.3_0": "Dhammaṁ sutvā jinaggassa,",
  "thi-ap19:48.4_0": "pabbajjaṁ samarocayiṁ.",
  "thi-ap19:49.1_0": "Anujāni na no tāto,",
  "thi-ap19:49.2_0": "agāreva tadā mayaṁ;",
  "thi-ap19:49.3_0": "Vīsavassasahassāni,",
  "thi-ap19:49.4_0": "vicarimha atanditā.",
  "thi-ap19:50.1_0": "Komāribrahmacariyaṁ,",
  "thi-ap19:50.2_0": "rājakaññā sukhedhitā;",
  "thi-ap19:50.3_0": "Buddhopaṭṭhānaniratā,",
  "thi-ap19:50.4_0": "muditā sattadhītaro.",
  "thi-ap19:51.1_0": "Samaṇī samaṇaguttā ca,",
  "thi-ap19:51.2_0": "bhikkhunī bhikkhudāyikā;",
  "thi-ap19:51.3_0": "Dhammā ceva sudhammā ca,",
  "thi-ap19:51.4_0": "sattamī saṅghadāyikā.",
  "thi-ap19:52.1_0": "Ahaṁ khemā ca sappaññā,",
  "thi-ap19:52.2_0": "paṭācārā ca kuṇḍalā;",
  "thi-ap19:52.3_0": "Kisāgotamī dhammadinnā,",
  "thi-ap19:52.4_0": "visākhā hoti sattamī.",
  "thi-ap19:53.1_0": "Tehi kammehi sukatehi,",
  "thi-ap19:53.2_0": "cetanāpaṇidhīhi ca;",
  "thi-ap19:53.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "thi-ap19:53.4_0": "tāvatiṁsamagacchahaṁ.",
  "thi-ap19:54.1_0": "Tato cutā manussesu,",
  "thi-ap19:54.2_0": "upapannā mahākule;",
  "thi-ap19:54.3_0": "Pītaṁ maṭṭhaṁ varaṁ dussaṁ,",
  "thi-ap19:54.4_0": "adaṁ arahato ahaṁ.",
  "thi-ap19:55.1_0": "Tato cutāriṭṭhapure,",
  "thi-ap19:55.2_0": "jātā vippakule ahaṁ;",
  "thi-ap19:55.3_0": "Dhītā tiriṭivacchassa,",
  "thi-ap19:55.4_0": "ummādantī manoharā.",
  "thi-ap19:56.1_0": "Tato cutā janapade,",
  "thi-ap19:56.2_0": "kule aññatare ahaṁ;",
  "thi-ap19:56.3_0": "Pasūtā nātiphītamhi,",
  "thi-ap19:56.4_0": "sāliṁ gopemahaṁ tadā.",
  "thi-ap19:57.1_0": "Disvā paccekasambuddhaṁ,",
  "thi-ap19:57.2_0": "pañcalājāsatānihaṁ;",
  "thi-ap19:57.3_0": "Datvā padumacchannāni,",
  "thi-ap19:57.4_0": "pañca puttasatānihaṁ.",
  "thi-ap19:58.1_0": "Patthayiṁ tepi patthesuṁ,",
  "thi-ap19:58.2_0": "madhuṁ datvā sayambhuno;",
  "thi-ap19:58.3_0": "Tato cutā araññehaṁ,",
  "thi-ap19:58.4_0": "ajāyiṁ padumodare.",
  "thi-ap19:59.1_0": "Kāsirañño mahesīhaṁ,",
  "thi-ap19:59.2_0": "hutvā sakkatapūjitā;",
  "thi-ap19:59.3_0": "Ajaniṁ rājaputtānaṁ,",
  "thi-ap19:59.4_0": "anūnaṁ satapañcakaṁ.",
  "thi-ap19:60.1_0": "Yadā te yobbanappattā,",
  "thi-ap19:60.2_0": "kīḷantā jalakīḷitaṁ;",
  "thi-ap19:60.3_0": "Disvā opattapadumaṁ,",
  "thi-ap19:60.4_0": "āsuṁ paccekanāyakā.",
  "thi-ap19:61.1_0": "Sāhaṁ tehi vinābhūtā,",
  "thi-ap19:61.2_0": "sutavīrehi sokinī;",
  "thi-ap19:61.3_0": "Cutā isigilipasse,",
  "thi-ap19:61.4_0": "gāmakamhi ajāyihaṁ.",
  "thi-ap19:62.1_0": "Yadā buddhā sutamatī,",
  "thi-ap19:62.2_0": "sutānaṁ bhattunopi ca;",
  "thi-ap19:62.3_0": "Yāguṁ ādāya gacchantī,",
  "thi-ap19:62.4_0": "aṭṭha paccekanāyake.",
  "thi-ap19:63.1_0": "Bhikkhāya gāmaṁ gacchante,",
  "thi-ap19:63.2_0": "disvā putte anussariṁ;",
  "thi-ap19:63.3_0": "Khīradhārā viniggacchi,",
  "thi-ap19:63.4_0": "tadā me puttapemasā.",
  "thi-ap19:64.1_0": "Tato tesaṁ adaṁ yāguṁ,",
  "thi-ap19:64.2_0": "pasannā sehi pāṇibhi;",
  "thi-ap19:64.3_0": "Tato cutāhaṁ tidasaṁ,",
  "thi-ap19:64.4_0": "nandanaṁ upapajjahaṁ.",
  "thi-ap19:65.1_0": "Anubhotvā sukhaṁ dukkhaṁ,",
  "thi-ap19:65.2_0": "saṁsaritvā bhavābhave;",
  "thi-ap19:65.3_0": "Tavatthāya mahāvīra,",
  "thi-ap19:65.4_0": "pariccattañca jīvitaṁ.",
  "thi-ap19:66.1_0": "Evaṁ bahuvidhaṁ dukkhaṁ,",
  "thi-ap19:66.2_0": "sampattī ca bahubbidhā;",
  "thi-ap19:66.3_0": "Pacchime bhave sampatte,",
  "thi-ap19:66.4_0": "jātā sāvatthiyaṁ pure.",
  "thi-ap19:67.1_0": "Mahādhanaseṭṭhikule,",
  "thi-ap19:67.2_0": "sukhite sajjite tathā;",
  "thi-ap19:67.3_0": "Nānāratanapajjote,",
  "thi-ap19:67.4_0": "sabbakāmasamiddhine.",
  "thi-ap19:68.1_0": "Sakkatā pūjitā ceva,",
  "thi-ap19:68.2_0": "mānitāpacitā tathā;",
  "thi-ap19:68.3_0": "Rūpasobhaggasampannā,",
  "thi-ap19:68.4_0": "kulesu atisakkatā.",
  "thi-ap19:69.1_0": "Atīva patthitā cāsiṁ,",
  "thi-ap19:69.2_0": "rūpabhogasirīhi ca;",
  "thi-ap19:69.3_0": "Patthitā seṭṭhiputtehi,",
  "thi-ap19:69.4_0": "anekehi satehipi.",
  "thi-ap19:70.1_0": "Agāraṁ pajahitvāna,",
  "thi-ap19:70.2_0": "pabbajiṁ anagāriyaṁ;",
  "thi-ap19:70.3_0": "Aḍḍhamāse asampatte,",
  "thi-ap19:70.4_0": "catusaccamapāpuṇiṁ.",
  "thi-ap19:71.1_0": "Iddhiyā abhinimmitvā,",
  "thi-ap19:71.2_0": "caturassaṁ rathaṁ ahaṁ;",
  "thi-ap19:71.3_0": "Buddhassa pāde vandissaṁ,",
  "thi-ap19:71.4_0": "lokanāthassa tādino.",
  "thi-ap19:72.1_0": "‘Supupphitaggaṁ upagamma pādapaṁ,",
  "thi-ap19:72.2_0": "Ekā tuvaṁ tiṭṭhasi sālamūle;",
  "thi-ap19:72.3_0": "Na cāpi te dutiyo atthi koci,",
  "thi-ap19:72.4_0": "Bāle na tvaṁ bhāyasi dhuttakānaṁ’.",
  "thi-ap19:73.1_0": "‘Sataṁ sahassānipi dhuttakānaṁ,",
  "thi-ap19:73.2_0": "Samāgatā edisakā bhaveyyuṁ;",
  "thi-ap19:73.3_0": "Lomaṁ na iñje na sampavedhe,",
  "thi-ap19:73.4_0": "Kiṁ me tuvaṁ māra karissaseko.",
  "thi-ap19:74.1_0": "Esā antaradhāyāmi,",
  "thi-ap19:74.2_0": "kucchiṁ vā pavisāmi te;",
  "thi-ap19:74.3_0": "Bhamukantarikāyampi,",
  "thi-ap19:74.4_0": "tiṭṭhantiṁ maṁ na dakkhasi.",
  "thi-ap19:75.1_0": "Cittasmiṁ vasībhūtāmhi,",
  "thi-ap19:75.2_0": "iddhipādā subhāvitā;",
  "thi-ap19:75.3_0": "Sabbabandhanamuttāmhi,",
  "thi-ap19:75.4_0": "na taṁ bhāyāmi āvuso.",
  "thi-ap19:76.1_0": "Sattisūlūpamā kāmā,",
  "thi-ap19:76.2_0": "khandhāsaṁ adhikuṭṭanā;",
  "thi-ap19:76.3_0": "Yaṁ tvaṁ kāmaratiṁ brūsi,",
  "thi-ap19:76.4_0": "aratī dāni sā mama.",
  "thi-ap19:77.1_0": "Sabbattha vihatā nandī,",
  "thi-ap19:77.2_0": "tamokhandho padālito;",
  "thi-ap19:77.3_0": "Evaṁ jānāhi pāpima,",
  "thi-ap19:77.4_0": "nihato tvamasi antaka’.",
  "thi-ap19:78.1_0": "Jino tamhi guṇe tuṭṭho,",
  "thi-ap19:78.2_0": "etadagge ṭhapesi maṁ;",
  "thi-ap19:78.3_0": "Aggā iddhimatīnanti,",
  "thi-ap19:78.4_0": "parisāsu vināyako.",
  "thi-ap19:79.1_0": "Pariciṇṇo mayā satthā,",
  "thi-ap19:79.2_0": "kataṁ buddhassa sāsanaṁ;",
  "thi-ap19:79.3_0": "Ohito garuko bhāro,",
  "thi-ap19:79.4_0": "bhavanettisamūhatā.",
  "thi-ap19:80.1_0": "Yassatthāya pabbajitā,",
  "thi-ap19:80.2_0": "agārasmānagāriyaṁ;",
  "thi-ap19:80.3_0": "So me attho anuppatto,",
  "thi-ap19:80.4_0": "sabbasaṁyojanakkhayo.",
  "thi-ap19:81.1_0": "Cīvaraṁ piṇḍapātañca,",
  "thi-ap19:81.2_0": "paccayaṁ sayanāsanaṁ;",
  "thi-ap19:81.3_0": "Khaṇena upanāmenti,",
  "thi-ap19:81.4_0": "sahassāni samantato.",
  "thi-ap19:82.1_0": "Kilesā jhāpitā mayhaṁ,",
  "thi-ap19:82.2_0": "…pe…",
  "thi-ap19:82.3_0": "viharāmi anāsavā.",
  "thi-ap19:83.1_0": "Svāgataṁ vata me āsi,",
  "thi-ap19:83.2_0": "…pe…",
  "thi-ap19:83.3_0": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap19:84.1_0": "Paṭisambhidā catasso,",
  "thi-ap19:84.2_0": "…pe…",
  "thi-ap19:84.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "thi-ap19:85.1_0": "Itthaṁ sudaṁ uppalavaṇṇā bhikkhunī imā gāthāyo abhāsitthāti.",
  "thi-ap19:86.1_0": "Uppalavaṇṇātheriyāpadānaṁ navamaṁ."
}