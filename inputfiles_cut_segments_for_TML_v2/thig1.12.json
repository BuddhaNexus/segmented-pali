{
  "thig1.12:0.1_0": "Therīgāthā",
  "thig1.12:0.2_0": "Ekakanipāta",
  "thig1.12:0.3_0": "12. Dhammadinnātherīgāthā",
  "thig1.12:1.1_0": "“Chandajātā avasāyī,",
  "thig1.12:1.2_0": "Manasā ca phuṭā siyā;",
  "thig1.12:1.3_0": "Kāmesu appaṭibaddhacittā,",
  "thig1.12:1.4_0": "Uddhaṁsotāti vuccatī”ti.",
  "thig1.12:2.1_0": "… Dhammadinnā therī …."
}