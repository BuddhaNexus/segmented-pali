{
  "ja235:0.1_0": "Jātaka",
  "ja235:0.2_0": "Dukanipāta",
  "ja235:0.3_0": "Upāhanavagga",
  "ja235:0.4_0": "5. Vacchanakhajātaka",
  "ja235:1.1_0": "“Sukhā gharā vacchanakha,",
  "ja235:1.2_0": "sahiraññā sabhojanā;",
  "ja235:1.3_0": "Yattha bhutvā pivitvā ca,",
  "ja235:1.4_0": "sayeyyātha anussuko”.",
  "ja235:2.1_0": "“Gharā nānīhamānassa,",
  "ja235:2.2_0": "Gharā nābhaṇato musā;",
  "ja235:2.3_0": "Gharā nādinnadaṇḍassa,",
  "ja235:2.4_0": "Paresaṁ anikubbato;",
  "ja235:2.5_0": "Evaṁ chiddaṁ durabhisambhavaṁ,",
  "ja235:2.6_0": "Ko gharaṁ paṭipajjatī”ti.",
  "ja235:3.1_0": "Vacchanakhajātakaṁ pañcamaṁ."
}