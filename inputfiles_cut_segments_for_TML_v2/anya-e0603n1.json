{
  "anya-e0603n1:0_0": "Buddhavandanā",
  "anya-e0603n1:1_0": "1.",
  "anya-e0603n1:2_0": "Devalokā cavitvāna,",
  "anya-e0603n1:3_0": "Mahāmāyāya kucchiyaṁ;",
  "anya-e0603n1:4_0": "Uppajji guruvāramhi,",
  "anya-e0603n1:5_0": "Vandetaṁ sakyapuṅgavaṁ.",
  "anya-e0603n1:6_0": "2.",
  "anya-e0603n1:7_0": "Dasamā saccaye neso,",
  "anya-e0603n1:8_0": "Vijāyi mātukucchito;",
  "anya-e0603n1:9_0": "Sukkavāre lumbiniyaṁ,",
  "anya-e0603n1:10_0": "Vande taṁ lokapūjitaṁ.",
  "anya-e0603n1:11_0": "3.",
  "anya-e0603n1:12_0": "Cakkavattisiriṁ hitvā,",
  "anya-e0603n1:13_0": "Mahāsinikkhamaṁ sudhī;",
  "anya-e0603n1:14_0": "Nikkhamī candavāramhi,",
  "anya-e0603n1:15_0": "Name taṁ munikuñjaraṁ.",
  "anya-e0603n1:16_0": "4.",
  "anya-e0603n1:17_0": "Assatthabodhimūlamhi,",
  "anya-e0603n1:18_0": "Pallaṅke aparājite;",
  "anya-e0603n1:19_0": "Patto sabbaññutaṁ nātho,",
  "anya-e0603n1:20_0": "Name taṁ budhavāsare.",
  "anya-e0603n1:21_0": "5.",
  "anya-e0603n1:22_0": "Pañcavaggiyamukhānaṁ,",
  "anya-e0603n1:23_0": "Devānaṁ migadāvane;",
  "anya-e0603n1:24_0": "Sorivāre dhammacakkaṁ,",
  "anya-e0603n1:25_0": "Vattesi taṁ jinaṁ name.",
  "anya-e0603n1:26_0": "6.",
  "anya-e0603n1:27_0": "Sabbasaṅkhatadhammānaṁ,",
  "anya-e0603n1:28_0": "Pakāsiyaaniccataṁ;",
  "anya-e0603n1:29_0": "Nibbutaṁaṅgavāramhi,",
  "anya-e0603n1:30_0": "Namādhiamatandadaṁ.",
  "anya-e0603n1:31_0": "7.",
  "anya-e0603n1:32_0": "Sūriyavāre soṇṇāya,",
  "anya-e0603n1:33_0": "Doṇiyā jinaviggaho;",
  "anya-e0603n1:34_0": "Agginujjalito tassa,",
  "anya-e0603n1:35_0": "Name dhātusarīrakaṁ."
}