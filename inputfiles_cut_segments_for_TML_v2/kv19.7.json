{
  "kv19.7:0.1_0": "Kathāvatthu",
  "kv19.7:0.2_0": "Mahāpaṇṇāsaka",
  "kv19.7:0.3_0": "Ekūnavīsatimavagga",
  "kv19.7:0.4_0": "Accantaniyāmakathā",
  "kv19.7:1.1_0": "Atthi puthujjanassa accantaniyāmatāti?",
  "kv19.7:1.2_0": "Āmantā.",
  "kv19.7:1.3_0": "Mātughātako accantaniyato, pitughātako …pe…",
  "kv19.7:1.4_0": "arahantaghātako …pe…",
  "kv19.7:1.5_0": "ruhiruppādako …pe…",
  "kv19.7:1.6_0": "saṅghabhedako accantaniyatoti?",
  "kv19.7:1.7_0": "Na hevaṁ vattabbe …pe….",
  "kv19.7:2.1_0": "Atthi puthujjanassa accantaniyāmatāti?",
  "kv19.7:2.2_0": "Āmantā.",
  "kv19.7:2.3_0": "Accantaniyatassa puggalassa vicikicchā uppajjeyyāti?",
  "kv19.7:2.4_0": "Āmantā.",
  "kv19.7:2.5_0": "Hañci accantaniyatassa puggalassa vicikicchā uppajjeyya, no ca vata re vattabbe—",
  "kv19.7:2.6_0": "“atthi puthujjanassa accantaniyāmatā”ti.",
  "kv19.7:3.1_0": "Accantaniyatassa puggalassa vicikicchā nuppajjeyyāti?",
  "kv19.7:3.2_0": "Āmantā.",
  "kv19.7:3.3_0": "Pahīnāti?",
  "kv19.7:3.4_0": "Na hevaṁ vattabbe …pe…",
  "kv19.7:3.5_0": "pahīnāti?",
  "kv19.7:3.6_0": "Āmantā.",
  "kv19.7:3.7_0": "Sotāpattimaggenāti?",
  "kv19.7:3.8_0": "Na hevaṁ vattabbe …pe…",
  "kv19.7:3.9_0": "sakadāgāmimaggena …pe…",
  "kv19.7:3.10_0": "anāgāmimaggena …pe…",
  "kv19.7:3.11_0": "arahattamaggenāti?",
  "kv19.7:3.12_0": "Na hevaṁ vattabbe …pe….",
  "kv19.7:4.1_0": "Katamena maggenāti?",
  "kv19.7:4.2_0": "Akusalena maggenāti.",
  "kv19.7:4.3_0": "Akusalo maggo niyyāniko khayagāmī bodhagāmī anāsavo …pe… asaṅkilesikoti?",
  "kv19.7:4.4_0": "Na hevaṁ vattabbe …pe…",
  "kv19.7:4.5_0": "nanu akusalo maggo aniyyāniko …pe…",
  "kv19.7:4.6_0": "saṅkilesikoti?",
  "kv19.7:4.7_0": "Āmantā.",
  "kv19.7:4.8_0": "Hañci akusalo maggo aniyyāniko …pe…",
  "kv19.7:4.9_0": "saṅkilesiko, no ca vata re vattabbe—",
  "kv19.7:4.10_0": "“accantaniyatassa puggalassa vicikicchā akusalena maggena pahīnā”ti.",
  "kv19.7:5.1_0": "Sassatadiṭṭhiyā niyatassa puggalassa ucchedadiṭṭhi uppajjeyyāti?",
  "kv19.7:5.2_0": "Āmantā.",
  "kv19.7:5.3_0": "Hañci sassatadiṭṭhiyā niyatassa puggalassa ucchedadiṭṭhi uppajjeyya, no ca vata re vattabbe—",
  "kv19.7:5.4_0": "“atthi puthujjanassa accantaniyāmatā”ti.",
  "kv19.7:6.1_0": "Sassatadiṭṭhiyā niyatassa puggalassa ucchedadiṭṭhi nuppajjeyyāti?",
  "kv19.7:6.2_0": "Āmantā.",
  "kv19.7:6.3_0": "Pahīnāti?",
  "kv19.7:6.4_0": "Na hevaṁ vattabbe …pe…",
  "kv19.7:6.5_0": "pahīnāti?",
  "kv19.7:6.6_0": "Āmantā.",
  "kv19.7:6.7_0": "Sotāpattimaggenāti?",
  "kv19.7:6.8_0": "Na hevaṁ vattabbe …pe…",
  "kv19.7:6.9_0": "sakadāgāmimaggena …pe…",
  "kv19.7:6.10_0": "anāgāmimaggena …pe…",
  "kv19.7:6.11_0": "arahattamaggenāti?",
  "kv19.7:6.12_0": "Na hevaṁ vattabbe …pe….",
  "kv19.7:7.1_0": "Katamena maggenāti?",
  "kv19.7:7.2_0": "Akusalena maggena.",
  "kv19.7:7.3_0": "Akusalo maggo …pe… no ca vata re vattabbe—",
  "kv19.7:7.4_0": "“sassatadiṭṭhiyā niyatassa puggalassa ucchedadiṭṭhi akusalena maggena pahīnā”ti.",
  "kv19.7:8.1_0": "Ucchedadiṭṭhiyā niyatassa puggalassa sassatadiṭṭhi uppajjeyyāti?",
  "kv19.7:8.2_0": "Āmantā.",
  "kv19.7:8.3_0": "Hañci ucchedadiṭṭhiyā niyatassa puggalassa sassatadiṭṭhi uppajjeyya, no ca vata re vattabbe—",
  "kv19.7:8.4_0": "“atthi puthujjanassa accantaniyāmatā”ti.",
  "kv19.7:9.1_0": "Ucchedadiṭṭhiyā niyatassa puggalassa sassatadiṭṭhi nuppajjeyyāti?",
  "kv19.7:9.2_0": "Āmantā.",
  "kv19.7:9.3_0": "Pahīnāti?",
  "kv19.7:9.4_0": "Na hevaṁ vattabbe …pe…",
  "kv19.7:9.5_0": "pahīnāti?",
  "kv19.7:9.6_0": "Āmantā.",
  "kv19.7:9.7_0": "Sotāpattimaggenāti?",
  "kv19.7:9.8_0": "Na hevaṁ vattabbe …pe…",
  "kv19.7:9.9_0": "sakadāgāmimaggena …pe…",
  "kv19.7:9.10_0": "anāgāmimaggena …pe…",
  "kv19.7:9.11_0": "arahattamaggenāti?",
  "kv19.7:9.12_0": "Na hevaṁ vattabbe …pe….",
  "kv19.7:10.1_0": "Katamena maggenāti?",
  "kv19.7:10.2_0": "Akusalena maggenāti.",
  "kv19.7:10.3_0": "Akusalo maggo …pe… no ca vata re vattabbe—",
  "kv19.7:10.4_0": "“ucchedadiṭṭhiyā niyatassa puggalassa sassatadiṭṭhi akusalena maggena pahīnā”ti.",
  "kv19.7:11.1_0": "Na vattabbaṁ—",
  "kv19.7:11.2_0": "“atthi puthujjanassa accantaniyāmatā”ti?",
  "kv19.7:11.3_0": "Āmantā.",
  "kv19.7:11.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv19.7:11.5_0": "“idha, bhikkhave, ekacco puggalo samannāgato hoti ekantakāḷakehi akusalehi dhammehi, so sakiṁ nimuggo nimuggova hotī”ti.",
  "kv19.7:11.6_0": "Attheva suttantoti?",
  "kv19.7:11.7_0": "Āmantā.",
  "kv19.7:11.8_0": "Tena hi atthi puthujjanassa accantaniyāmatāti.",
  "kv19.7:12.1_0": "Vuttaṁ bhagavatā—",
  "kv19.7:12.2_0": "“idha, bhikkhave, ekacco puggalo samannāgato hoti ekantakāḷakehi akusalehi dhammehi, so sakiṁ nimuggo nimuggova hotī”ti katvā tena ca kāraṇena atthi puthujjanassa accantaniyāmatāti?",
  "kv19.7:12.3_0": "Āmantā.",
  "kv19.7:12.4_0": "Vuttaṁ bhagavatā—",
  "kv19.7:12.5_0": "“idha, bhikkhave, ekacco puggalo ummujjitvā nimujjatī”ti.",
  "kv19.7:12.6_0": "Attheva suttantoti?",
  "kv19.7:12.7_0": "Āmantā.",
  "kv19.7:12.8_0": "Sabbakālaṁ ummujjitvā nimujjatīti?",
  "kv19.7:12.9_0": "Na hevaṁ vattabbe …pe….",
  "kv19.7:13.1_0": "Vuttaṁ bhagavatā—",
  "kv19.7:13.2_0": "“idha, bhikkhave, ekacco puggalo samannāgato hoti ekantakāḷakehi akusalehi dhammehi, so sakiṁ nimuggo nimuggova hotī”ti—",
  "kv19.7:13.3_0": "katvā tena ca kāraṇena atthi puthujjanassa accantaniyāmatāti?",
  "kv19.7:13.4_0": "Āmantā.",
  "kv19.7:13.5_0": "Vuttaṁ bhagavatā—",
  "kv19.7:13.6_0": "“idha, bhikkhave, ekacco puggalo ummujjitvā ṭhito hoti, ummujjitvā vipassati viloketi, ummujjitvā patarati, ummujjitvā patigādhappatto hotī”ti.",
  "kv19.7:13.7_0": "Attheva suttantoti?",
  "kv19.7:13.8_0": "Āmantā.",
  "kv19.7:13.9_0": "Sabbakālaṁ ummujjitvā patigādhappatto hotīti?",
  "kv19.7:13.10_0": "Na hevaṁ vattabbe …pe….",
  "kv19.7:13.11_0": "Accantaniyāmakathā niṭṭhitā."
}