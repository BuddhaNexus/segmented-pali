{
  "tha-ap162:0.1_0": "Therāpadāna",
  "tha-ap162:0.2_0": "Bandhujīvakavagga",
  "tha-ap162:0.3_0": "10. Kumudadāyakattheraapadāna",
  "tha-ap162:1.1_0": "“Himavantassāvidūre,",
  "tha-ap162:1.2_0": "mahājātassaro ahu;",
  "tha-ap162:1.3_0": "Padumuppalasañchanno,",
  "tha-ap162:1.4_0": "puṇḍarīkasamotthaṭo.",
  "tha-ap162:2.1_0": "Kukuttho nāma nāmena,",
  "tha-ap162:2.2_0": "tatthāsiṁ sakuṇo tadā;",
  "tha-ap162:2.3_0": "Sīlavā buddhisampanno,",
  "tha-ap162:2.4_0": "puññāpuññesu kovido.",
  "tha-ap162:3.1_0": "Padumuttaro lokavidū,",
  "tha-ap162:3.2_0": "āhutīnaṁ paṭiggaho;",
  "tha-ap162:3.3_0": "Jātassarassāvidūre,",
  "tha-ap162:3.4_0": "sañcarittha mahāmuni.",
  "tha-ap162:4.1_0": "Jalajaṁ kumudaṁ chetvā,",
  "tha-ap162:4.2_0": "upanesiṁ mahesino;",
  "tha-ap162:4.3_0": "Mama saṅkappamaññāya,",
  "tha-ap162:4.4_0": "paṭiggahi mahāmuni.",
  "tha-ap162:5.1_0": "Tañca dānaṁ daditvāna,",
  "tha-ap162:5.2_0": "sukkamūlena codito;",
  "tha-ap162:5.3_0": "Kappānaṁ satasahassaṁ,",
  "tha-ap162:5.4_0": "duggatiṁ nupapajjahaṁ.",
  "tha-ap162:6.1_0": "Soḷaseto kappasate,",
  "tha-ap162:6.2_0": "āsuṁ varuṇanāmakā;",
  "tha-ap162:6.3_0": "Aṭṭha ete janādhipā,",
  "tha-ap162:6.4_0": "cakkavattī mahabbalā.",
  "tha-ap162:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap162:7.2_0": "…pe…",
  "tha-ap162:7.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap162:8.1_0": "Itthaṁ sudaṁ āyasmā kumudadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap162:9.1_0": "Kumudadāyakattherassāpadānaṁ dasamaṁ.",
  "tha-ap162:10.1_0": "Bandhujīvakavaggo soḷasamo.",
  "tha-ap162:11.1_0": "Tassuddānaṁ",
  "tha-ap162:12.1_0": "Bandhujīvo tambapupphī,",
  "tha-ap162:12.2_0": "vīthikakkārupupphiyo;",
  "tha-ap162:12.3_0": "Mandāravo kadambī ca,",
  "tha-ap162:12.4_0": "sūlako nāgapupphiyo;",
  "tha-ap162:12.5_0": "Punnāgo komudī gāthā,",
  "tha-ap162:12.6_0": "chappaññāsa pakittitāti."
}