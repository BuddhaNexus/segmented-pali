{
  "ja97:0.1_0": "Jātaka",
  "ja97:0.2_0": "Ekakanipāta",
  "ja97:0.3_0": "Littavagga",
  "ja97:0.4_0": "7. Nāmasiddhijātaka",
  "ja97:1.1_0": "“Jīvakañca mataṁ disvā,",
  "ja97:1.2_0": "dhanapāliñca duggataṁ;",
  "ja97:1.3_0": "Panthakañca vane mūḷhaṁ,",
  "ja97:1.4_0": "pāpako punarāgato”ti.",
  "ja97:2.1_0": "Nāmasiddhijātakaṁ sattamaṁ."
}