{
  "tha-ap391:0.1_0": "Therāpadāna",
  "tha-ap391:0.2_0": "Avaṭaphalavagga",
  "tha-ap391:0.3_0": "9 Soṇakoṭivīsattheraapadāna",
  "tha-ap391:1.1_0": "“Vipassino pāvacane,",
  "tha-ap391:1.2_0": "ekaṁ leṇaṁ mayā kataṁ;",
  "tha-ap391:1.3_0": "Cātuddisassa saṅghassa,",
  "tha-ap391:1.4_0": "bandhumārājadhāniyā.",
  "tha-ap391:2.1_0": "Dussehi bhūmiṁ leṇassa,",
  "tha-ap391:2.2_0": "santharitvā pariccajiṁ;",
  "tha-ap391:2.3_0": "Udaggacitto sumano,",
  "tha-ap391:2.4_0": "akāsiṁ paṇidhiṁ tadā.",
  "tha-ap391:3.1_0": "Ārādhayeyyaṁ sambuddhaṁ,",
  "tha-ap391:3.2_0": "pabbajjañca labheyyahaṁ;",
  "tha-ap391:3.3_0": "Anuttarañca nibbānaṁ,",
  "tha-ap391:3.4_0": "phuseyyaṁ santimuttamaṁ.",
  "tha-ap391:4.1_0": "Teneva sukkamūlena,",
  "tha-ap391:4.2_0": "kappe navuti saṁsariṁ;",
  "tha-ap391:4.3_0": "Devabhūto manusso ca,",
  "tha-ap391:4.4_0": "katapuñño virocahaṁ.",
  "tha-ap391:5.1_0": "Tato kammāvasesena,",
  "tha-ap391:5.2_0": "idha pacchimake bhave;",
  "tha-ap391:5.3_0": "Campāyaṁ aggaseṭṭhissa,",
  "tha-ap391:5.4_0": "jātomhi ekaputtako.",
  "tha-ap391:6.1_0": "Jātamattassa me sutvā,",
  "tha-ap391:6.2_0": "pitu chando ayaṁ ahu;",
  "tha-ap391:6.3_0": "Dadāmahaṁ kumārassa,",
  "tha-ap391:6.4_0": "vīsakoṭī anūnakā.",
  "tha-ap391:7.1_0": "Caturaṅgulā ca me lomā,",
  "tha-ap391:7.2_0": "jātā pādatale ubho;",
  "tha-ap391:7.3_0": "Sukhumā mudusamphassā,",
  "tha-ap391:7.4_0": "tūlāpicusamā subhā.",
  "tha-ap391:8.1_0": "Atītā navuti kappā,",
  "tha-ap391:8.2_0": "ayaṁ eko ca uttari;",
  "tha-ap391:8.3_0": "Nābhijānāmi nikkhitte,",
  "tha-ap391:8.4_0": "pāde bhūmyā asanthate.",
  "tha-ap391:9.1_0": "Ārādhito me sambuddho,",
  "tha-ap391:9.2_0": "pabbajiṁ anagāriyaṁ;",
  "tha-ap391:9.3_0": "Arahattañca me pattaṁ,",
  "tha-ap391:9.4_0": "sītibhūtomhi nibbuto.",
  "tha-ap391:10.1_0": "Aggo āraddhavīriyānaṁ,",
  "tha-ap391:10.2_0": "niddiṭṭho sabbadassinā;",
  "tha-ap391:10.3_0": "Khīṇāsavomhi arahā,",
  "tha-ap391:10.4_0": "chaḷabhiñño mahiddhiko.",
  "tha-ap391:11.1_0": "Ekanavutito kappe,",
  "tha-ap391:11.2_0": "yaṁ dānamadadiṁ tadā;",
  "tha-ap391:11.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap391:11.4_0": "leṇadānassidaṁ phalaṁ.",
  "tha-ap391:12.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap391:12.2_0": "…pe…",
  "tha-ap391:12.3_0": "viharāmi anāsavo.",
  "tha-ap391:13.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap391:13.2_0": "…pe…",
  "tha-ap391:13.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap391:14.1_0": "Paṭisambhidā catasso,",
  "tha-ap391:14.2_0": "…pe…",
  "tha-ap391:14.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap391:15.1_0": "Thero koṭivīso soṇo,",
  "tha-ap391:15.2_0": "bhikkhusaṅghassa aggato;",
  "tha-ap391:15.3_0": "Pañhaṁ puṭṭho viyākāsi,",
  "tha-ap391:15.4_0": "anotatte mahāsareti.",
  "tha-ap391:16.1_0": "Itthaṁ sudaṁ āyasmā soṇo koṭivīso thero imā gāthāyo abhāsitthāti.",
  "tha-ap391:17.1_0": "Soṇakoṭivīsattherassāpadānaṁ navamaṁ."
}