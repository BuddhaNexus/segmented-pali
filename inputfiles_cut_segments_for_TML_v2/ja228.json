{
  "ja228:0.1_0": "Jātaka",
  "ja228:0.2_0": "Dukanipāta",
  "ja228:0.3_0": "Kāsāvavagga",
  "ja228:0.4_0": "8. Kāmanītajātaka",
  "ja228:1.1_0": "“Tayo giriṁ antaraṁ kāmayāmi,",
  "ja228:1.2_0": "Pañcālā kuruyo kekake ca;",
  "ja228:1.3_0": "Tatuttariṁ brāhmaṇa kāmayāmi,",
  "ja228:1.4_0": "Tikiccha maṁ brāhmaṇa kāmanītaṁ”.",
  "ja228:2.1_0": "“Kaṇhāhidaṭṭhassa karonti heke,",
  "ja228:2.2_0": "Amanussapaviṭṭhassa karonti paṇḍitā;",
  "ja228:2.3_0": "Na kāmanītassa karoti koci,",
  "ja228:2.4_0": "Okkantasukkassa hi kā tikicchā”ti.",
  "ja228:3.1_0": "Kāmanītajātakaṁ aṭṭhamaṁ."
}