{
  "tha-ap38:0.1_0": "Therāpadāna",
  "tha-ap38:0.2_0": "Kuṇḍadhānavagga",
  "tha-ap38:0.3_0": "6 Adhimuttattheraapadāna",
  "tha-ap38:1.1_0": "“Nibbute lokanāthamhi,",
  "tha-ap38:1.2_0": "atthadassīnaruttame;",
  "tha-ap38:1.3_0": "Upaṭṭhahiṁ bhikkhusaṅghaṁ,",
  "tha-ap38:1.4_0": "vippasannena cetasā.",
  "tha-ap38:2.1_0": "Nimantetvā bhikkhusaṅghaṁ,",
  "tha-ap38:2.2_0": "Ujubhūtaṁ samāhitaṁ;",
  "tha-ap38:2.3_0": "Ucchunā maṇḍapaṁ katvā,",
  "tha-ap38:2.4_0": "Bhojesiṁ saṅghamuttamaṁ.",
  "tha-ap38:3.1_0": "Yaṁ yaṁ yonupapajjāmi,",
  "tha-ap38:3.2_0": "devattaṁ atha mānusaṁ;",
  "tha-ap38:3.3_0": "Sabbe satte abhibhomi,",
  "tha-ap38:3.4_0": "puññakammassidaṁ phalaṁ.",
  "tha-ap38:4.1_0": "Aṭṭhārase kappasate,",
  "tha-ap38:4.2_0": "yaṁ dānamadadiṁ tadā;",
  "tha-ap38:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap38:4.4_0": "ucchudānassidaṁ phalaṁ.",
  "tha-ap38:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap38:5.2_0": "…pe…",
  "tha-ap38:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap38:6.1_0": "Itthaṁ sudaṁ āyasmā adhimutto thero imā gāthāyo abhāsitthāti.",
  "tha-ap38:7.1_0": "Adhimuttattherassāpadānaṁ chaṭṭhaṁ."
}