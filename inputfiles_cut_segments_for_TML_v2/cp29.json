{
  "cp29:0.1_0": "Cariyāpiṭaka",
  "cp29:0.2_0": "Yudhañjayavagga",
  "cp29:0.3_0": "Saccapāramī 3",
  "cp29:0.4_0": "Vaṭṭapotakacariya",
  "cp29:1.1_0": "“Punāparaṁ yadā homi,",
  "cp29:1.2_0": "magadhe vaṭṭapotako;",
  "cp29:1.3_0": "Ajātapakkho taruṇo,",
  "cp29:1.4_0": "maṁsapesi kulāvake.",
  "cp29:2.1_0": "Mukhatuṇḍakenāharitvā,",
  "cp29:2.2_0": "mātā posayatī mamaṁ;",
  "cp29:2.3_0": "Tassā phassena jīvāmi,",
  "cp29:2.4_0": "natthi me kāyikaṁ balaṁ.",
  "cp29:3.1_0": "Saṁvacchare gimhasamaye,",
  "cp29:3.2_0": "davaḍāho padippati;",
  "cp29:3.3_0": "Upagacchati amhākaṁ,",
  "cp29:3.4_0": "pāvako kaṇhavattanī.",
  "cp29:4.1_0": "Dhamadhamāiti evaṁ,",
  "cp29:4.2_0": "saddāyanto mahāsikhī;",
  "cp29:4.3_0": "Anupubbena jhāpento,",
  "cp29:4.4_0": "aggi mamamupāgami.",
  "cp29:5.1_0": "Aggivegabhayātītā,",
  "cp29:5.2_0": "tasitā mātāpitā mama;",
  "cp29:5.3_0": "Kulāvake maṁ chaḍḍetvā,",
  "cp29:5.4_0": "attānaṁ parimocayuṁ.",
  "cp29:6.1_0": "Pāde pakkhe pajahāmi,",
  "cp29:6.2_0": "natthi me kāyikaṁ balaṁ;",
  "cp29:6.3_0": "Sohaṁ agatiko tattha,",
  "cp29:6.4_0": "evaṁ cintesahaṁ tadā.",
  "cp29:7.1_0": "‘Yesāhaṁ upadhāveyyaṁ,",
  "cp29:7.2_0": "bhīto tasitavedhito;",
  "cp29:7.3_0": "Te maṁ ohāya pakkantā,",
  "cp29:7.4_0": "kathaṁ me ajja kātave.",
  "cp29:8.1_0": "Atthi loke sīlaguṇo,",
  "cp29:8.2_0": "saccaṁ soceyyanuddayā;",
  "cp29:8.3_0": "Tena saccena kāhāmi,",
  "cp29:8.4_0": "saccakiriyamuttamaṁ.",
  "cp29:9.1_0": "Āvejjetvā dhammabalaṁ,",
  "cp29:9.2_0": "saritvā pubbake jine;",
  "cp29:9.3_0": "Saccabalamavassāya,",
  "cp29:9.4_0": "saccakiriyamakāsahaṁ.",
  "cp29:10.1_0": "Santi pakkhā apatanā,",
  "cp29:10.2_0": "santi pādā avañcanā;",
  "cp29:10.3_0": "Mātāpitā ca nikkhantā,",
  "cp29:10.4_0": "jātaveda paṭikkama’.",
  "cp29:11.1_0": "Sahasacce kate mayhaṁ,",
  "cp29:11.2_0": "Mahāpajjalito sikhī;",
  "cp29:11.3_0": "Vajjesi soḷasakarīsāni,",
  "cp29:11.4_0": "Udakaṁ patvā yathā sikhī;",
  "cp29:11.5_0": "Saccena me samo natthi,",
  "cp29:11.6_0": "Esā me saccapāramī”ti.",
  "cp29:12.1_0": "Vaṭṭapotakacariyaṁ navamaṁ."
}