{
  "ja129:0.1_0": "Jātaka",
  "ja129:0.2_0": "Ekakanipāta",
  "ja129:0.3_0": "Kusanāḷivagga",
  "ja129:0.4_0": "9. Aggikabhāradvājajātaka",
  "ja129:1.1_0": "“Nāyaṁ sikhā puññahetu,",
  "ja129:1.2_0": "ghāsahetu ayaṁ sikhā;",
  "ja129:1.3_0": "Nāguṭṭhigaṇanaṁ yāti,",
  "ja129:1.4_0": "alaṁ te hotu aggikā”ti.",
  "ja129:2.1_0": "Aggikabhāradvājajātakaṁ navamaṁ."
}