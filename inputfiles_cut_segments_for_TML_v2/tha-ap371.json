{
  "tha-ap371:0.1_0": "Therāpadāna",
  "tha-ap371:0.2_0": "Mandāravapupphiyavagga",
  "tha-ap371:0.3_0": "9. Timirapupphiyattheraapadāna",
  "tha-ap371:1.1_0": "“Candabhāgānadītīre,",
  "tha-ap371:1.2_0": "anusotaṁ vajāmahaṁ;",
  "tha-ap371:1.3_0": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap371:1.4_0": "sālarājaṁva phullitaṁ.",
  "tha-ap371:2.1_0": "Pasannacitto sumano,",
  "tha-ap371:2.2_0": "paccekamunimuttamaṁ;",
  "tha-ap371:2.3_0": "Gahetvā timiraṁ pupphaṁ,",
  "tha-ap371:2.4_0": "matthake okiriṁ ahaṁ.",
  "tha-ap371:3.1_0": "Ekanavutito kappe,",
  "tha-ap371:3.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap371:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap371:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap371:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap371:4.2_0": "…pe…",
  "tha-ap371:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap371:5.1_0": "Itthaṁ sudaṁ āyasmā timirapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap371:6.1_0": "Timirapupphiyattherassāpadānaṁ navamaṁ."
}