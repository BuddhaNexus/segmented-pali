{
  "ja218:0.1_0": "Jātaka",
  "ja218:0.2_0": "Dukanipāta",
  "ja218:0.3_0": "Bīraṇathambhavagga",
  "ja218:0.4_0": "8. Kūṭavāṇijajātaka",
  "ja218:1.1_0": "“Saṭhassa sāṭheyyamidaṁ sucintitaṁ,",
  "ja218:1.2_0": "Paccoḍḍitaṁ paṭikūṭassa kūṭaṁ;",
  "ja218:1.3_0": "Phālañce khādeyyuṁ mūsikā,",
  "ja218:1.4_0": "Kasmā kumāraṁ kulalā na hareyyuṁ.",
  "ja218:2.1_0": "Kūṭassa hi santi kūṭakūṭā,",
  "ja218:2.2_0": "Bhavati cāpi nikatino nikatyā;",
  "ja218:2.3_0": "Dehi puttanaṭṭha phālanaṭṭhassa phālaṁ,",
  "ja218:2.4_0": "Mā te puttamahāsi phālanaṭṭho”ti.",
  "ja218:3.1_0": "Kūṭavāṇijajātakaṁ aṭṭhamaṁ."
}