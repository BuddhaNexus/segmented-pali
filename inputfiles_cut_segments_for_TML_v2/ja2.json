{
  "ja2:0.1_0": "Jātaka",
  "ja2:0.2_0": "Ekakanipāta",
  "ja2:0.3_0": "Apaṇṇakavagga",
  "ja2:0.4_0": "2. Vaṇṇupathajātaka",
  "ja2:1.1_0": "“Akilāsuno vaṇṇupathe khaṇantā,",
  "ja2:1.2_0": "Udaṅgaṇe tattha papaṁ avinduṁ;",
  "ja2:1.3_0": "Evaṁ munī viriyabalūpapanno,",
  "ja2:1.4_0": "Akilāsu vinde hadayassa santin”ti.",
  "ja2:2.1_0": "Vaṇṇupathajātakaṁ dutiyaṁ."
}