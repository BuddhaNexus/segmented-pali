{
  "ja452:0.1_0": "Jātaka",
  "ja452:0.2_0": "Dasakanipāta",
  "ja452:0.3_0": "Catudvāravagga",
  "ja452:0.4_0": "14. Bhūripaññajātaka",
  "ja452:1.1_0": "“Saccaṁ kira tvaṁ api bhūripañña,",
  "ja452:1.2_0": "Yā tādisī sīri dhitī matī ca;",
  "ja452:1.3_0": "Na tāyatebhāvavasūpanitaṁ,",
  "ja452:1.4_0": "Yo yavakaṁ bhuñjasi appasūpaṁ”.",
  "ja452:2.1_0": "“Sukhaṁ dukkhena paripācayanto,",
  "ja452:2.2_0": "Kālā kālaṁ vicinaṁ chandachanno;",
  "ja452:2.3_0": "Atthassa dvārāni avāpuranto,",
  "ja452:2.4_0": "Tenāhaṁ tussāmi yavodanena.",
  "ja452:3.1_0": "Kālañca ñatvā abhijīhanāya,",
  "ja452:3.2_0": "Mantehi atthaṁ paripācayitvā;",
  "ja452:3.3_0": "Vijambhissaṁ sīhavijambhitāni,",
  "ja452:3.4_0": "Tāyiddhiyā dakkhasi maṁ punāpi”.",
  "ja452:4.1_0": "“Sukhīpi heke na karonti pāpaṁ,",
  "ja452:4.2_0": "Avaṇṇasaṁsaggabhayā puneke;",
  "ja452:4.3_0": "Pahū samāno vipulatthacintī,",
  "ja452:4.4_0": "Kiṅkāraṇā me na karosi dukkhaṁ”.",
  "ja452:5.1_0": "“Na paṇḍitā attasukhassa hetu,",
  "ja452:5.2_0": "Pāpāni kammāni samācaranti;",
  "ja452:5.3_0": "Dukkhena phuṭṭhā khalitāpi santā,",
  "ja452:5.4_0": "Chandā ca dosā na jahanti dhammaṁ”.",
  "ja452:6.1_0": "“Yena kenaci vaṇṇena,",
  "ja452:6.2_0": "mudunā dāruṇena vā;",
  "ja452:6.3_0": "Uddhare dīnamattānaṁ,",
  "ja452:6.4_0": "pacchā dhammaṁ samācare”.",
  "ja452:7.1_0": "“Yassa rukkhassa chāyāya,",
  "ja452:7.2_0": "nisīdeyya sayeyya vā;",
  "ja452:7.3_0": "Na tassa sākhaṁ bhañjeyya,",
  "ja452:7.4_0": "mittadubbho hi pāpako.",
  "ja452:8.1_0": "Yassāpi dhammaṁ puriso vijaññā,",
  "ja452:8.2_0": "Ye cassa kaṅkhaṁ vinayanti santo;",
  "ja452:8.3_0": "Taṁ hissa dīpañca parāyanañca,",
  "ja452:8.4_0": "Na tena mettiṁ jarayetha pañño”.",
  "ja452:9.1_0": "“Alaso gihī kāmabhogī na sādhu,",
  "ja452:9.2_0": "Asaññato pabbajito na sādhu;",
  "ja452:9.3_0": "Rājā na sādhu anisammakārī,",
  "ja452:9.4_0": "Yo paṇḍito kodhano taṁ na sādhu.",
  "ja452:10.1_0": "Nisamma khattiyo kayirā,",
  "ja452:10.2_0": "nānisamma disampati;",
  "ja452:10.3_0": "Nisammakārino rāja,",
  "ja452:10.4_0": "yaso kitti ca vaḍḍhatī”ti.",
  "ja452:11.1_0": "Bhūripaññajātakaṁ cuddasamaṁ."
}