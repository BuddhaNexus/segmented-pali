{
  "tha-ap251:0.1_0": "Therāpadāna",
  "tha-ap251:0.2_0": "Tuvaradāyakavagga",
  "tha-ap251:0.3_0": "9 Bimbijāliyattheraapadāna",
  "tha-ap251:1.1_0": "“Padumuttaro nāma jino,",
  "tha-ap251:1.2_0": "sayambhū aggapuggalo;",
  "tha-ap251:1.3_0": "Catusaccaṁ pakāseti,",
  "tha-ap251:1.4_0": "dīpeti amataṁ padaṁ.",
  "tha-ap251:2.1_0": "Bimbijālakapupphāni,",
  "tha-ap251:2.2_0": "puthu katvānahaṁ tadā;",
  "tha-ap251:2.3_0": "Buddhassa abhiropesiṁ,",
  "tha-ap251:2.4_0": "dvipadindassa tādino.",
  "tha-ap251:3.1_0": "Aṭṭhasaṭṭhimhito kappe,",
  "tha-ap251:3.2_0": "caturo kiñjakesarā;",
  "tha-ap251:3.3_0": "Sattaratanasampannā,",
  "tha-ap251:3.4_0": "cakkavattī mahabbalā.",
  "tha-ap251:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap251:4.2_0": "…pe…",
  "tha-ap251:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap251:5.1_0": "Itthaṁ sudaṁ āyasmā bimbijāliyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap251:6.1_0": "Bimbijāliyattherassāpadānaṁ navamaṁ."
}