{
  "tha-ap529:0.1_0": "Therāpadāna",
  "tha-ap529:0.2_0": "Tiṇadāyakavagga",
  "tha-ap529:0.3_0": "7. Girinelapūjakattheraapadāna",
  "tha-ap529:1.1_0": "“Migaluddo pure āsiṁ,",
  "tha-ap529:1.2_0": "vipine vicaraṁ ahaṁ;",
  "tha-ap529:1.3_0": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap529:1.4_0": "sabbadhammāna pāraguṁ.",
  "tha-ap529:2.1_0": "Tasmiṁ mahākāruṇike,",
  "tha-ap529:2.2_0": "sabbasattahite rate;",
  "tha-ap529:2.3_0": "Pasannacitto sumano,",
  "tha-ap529:2.4_0": "nelapupphamapūjayiṁ.",
  "tha-ap529:3.1_0": "Ekatiṁse ito kappe,",
  "tha-ap529:3.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap529:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap529:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap529:4.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap529:4.2_0": "…pe…",
  "tha-ap529:4.3_0": "viharāmi anāsavo.",
  "tha-ap529:5.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap529:5.2_0": "…pe…",
  "tha-ap529:5.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap529:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap529:6.2_0": "…pe…",
  "tha-ap529:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap529:7.1_0": "Itthaṁ sudaṁ āyasmā girinelapūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap529:8.1_0": "Girinelapūjakattherassāpadānaṁ sattamaṁ."
}