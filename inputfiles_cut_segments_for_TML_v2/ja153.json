{
  "ja153:0.1_0": "Jātaka",
  "ja153:0.2_0": "Dukanipāta",
  "ja153:0.3_0": "Daḷhavagga",
  "ja153:0.4_0": "3. Sūkarajātaka",
  "ja153:1.1_0": "“Catuppado ahaṁ samma,",
  "ja153:1.2_0": "tvampi samma catuppado;",
  "ja153:1.3_0": "Ehi samma nivattassu,",
  "ja153:1.4_0": "kiṁ nu bhīto palāyasi”.",
  "ja153:2.1_0": "“Asuci pūtilomosi,",
  "ja153:2.2_0": "duggandho vāsi sūkara;",
  "ja153:2.3_0": "Sace yujjhitukāmosi,",
  "ja153:2.4_0": "jayaṁ samma dadāmi te”ti.",
  "ja153:3.1_0": "Sūkarajātakaṁ tatiyaṁ."
}