{
  "ja21:0.1_0": "Jātaka",
  "ja21:0.2_0": "Ekakanipāta",
  "ja21:0.3_0": "Kuruṅgavagga",
  "ja21:0.4_0": "1. Kuruṅgamigajātaka",
  "ja21:1.1_0": "“Ñātametaṁ kuruṅgassa,",
  "ja21:1.2_0": "yaṁ tvaṁ sepaṇṇi siyyasi;",
  "ja21:1.3_0": "Aññaṁ sepaṇṇi gacchāmi,",
  "ja21:1.4_0": "na me te ruccate phalan”ti.",
  "ja21:2.1_0": "Kuruṅgamigajātakaṁ paṭhamaṁ."
}