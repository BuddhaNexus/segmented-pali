{
  "ja229:0.1_0": "Jātaka",
  "ja229:0.2_0": "Dukanipāta",
  "ja229:0.3_0": "Kāsāvavagga",
  "ja229:0.4_0": "9. Palāyitajātaka",
  "ja229:1.1_0": "“Gajaggameghehi hayaggamālibhi,",
  "ja229:1.2_0": "Rathūmijātehi sarābhivassebhi;",
  "ja229:1.3_0": "Tharuggahāvaṭṭadaḷhappahāribhi,",
  "ja229:1.4_0": "Parivāritā takkasilā samantato.",
  "ja229:2.1_0": "Abhidhāvatha cūpadhāvatha ca,",
  "ja229:2.2_0": "Vividhā vināditā vadantibhi;",
  "ja229:2.3_0": "Vattatajja tumulo ghoso yathā,",
  "ja229:2.4_0": "Vijjulatā jaladharassa gajjato”ti.",
  "ja229:3.1_0": "Palāyitajātakaṁ navamaṁ."
}