{
  "ja333:0.1_0": "Jātaka",
  "ja333:0.2_0": "Catukkanipāta",
  "ja333:0.3_0": "Kokilavagga",
  "ja333:0.4_0": "3. Pakkagodhajātaka",
  "ja333:1.1_0": "“Tadeva me tvaṁ vidito,",
  "ja333:1.2_0": "vanamajjhe rathesabha;",
  "ja333:1.3_0": "Yassa te khaggabaddhassa,",
  "ja333:1.4_0": "sannaddhassa tirīṭino;",
  "ja333:1.5_0": "Assatthadumasākhāya,",
  "ja333:1.6_0": "pakkā godhā palāyatha”.",
  "ja333:2.1_0": "“Name namantassa bhaje bhajantaṁ,",
  "ja333:2.2_0": "Kiccānukubbassa kareyya kiccaṁ;",
  "ja333:2.3_0": "Nānatthakāmassa kareyya atthaṁ,",
  "ja333:2.4_0": "Asambhajantampi na sambhajeyya.",
  "ja333:3.1_0": "Caje cajantaṁ vanathaṁ na kayirā,",
  "ja333:3.2_0": "Apetacittena na sambhajeyya;",
  "ja333:3.3_0": "Dijo dumaṁ khīṇaphalanti ñatvā,",
  "ja333:3.4_0": "Aññaṁ samekkheyya mahā hi loko”.",
  "ja333:4.1_0": "“So te karissāmi yathānubhāvaṁ,",
  "ja333:4.2_0": "Kataññutaṁ khattiye pekkhamāno;",
  "ja333:4.3_0": "Sabbañca te issariyaṁ dadāmi,",
  "ja333:4.4_0": "Yassicchasī tassa tuvaṁ dadāmī”ti.",
  "ja333:5.1_0": "Pakkagodhajātakaṁ tatiyaṁ."
}