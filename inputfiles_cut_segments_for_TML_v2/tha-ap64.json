{
  "tha-ap64:0.1_0": "Therāpadāna",
  "tha-ap64:0.2_0": "Sakacintaniyavagga",
  "tha-ap64:0.3_0": "2 Avopupphiyattheraapadāna",
  "tha-ap64:1.1_0": "“Vihārā abhinikkhamma,",
  "tha-ap64:1.2_0": "abbhuṭṭhāsi ca caṅkame;",
  "tha-ap64:1.3_0": "Catusaccaṁ pakāsanto,",
  "tha-ap64:1.4_0": "desento amataṁ padaṁ.",
  "tha-ap64:2.1_0": "Sikhissa giramaññāya,",
  "tha-ap64:2.2_0": "buddhaseṭṭhassa tādino;",
  "tha-ap64:2.3_0": "Nānāpupphaṁ gahetvāna,",
  "tha-ap64:2.4_0": "ākāsamhi samokiriṁ.",
  "tha-ap64:3.1_0": "Tena kammena dvipadinda,",
  "tha-ap64:3.2_0": "lokajeṭṭha narāsabha;",
  "tha-ap64:3.3_0": "Pattomhi acalaṁ ṭhānaṁ,",
  "tha-ap64:3.4_0": "hitvā jayaparājayaṁ.",
  "tha-ap64:4.1_0": "Ekattiṁse ito kappe,",
  "tha-ap64:4.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap64:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap64:4.4_0": "pupphapūjāyidaṁ phalaṁ.",
  "tha-ap64:5.1_0": "Ito vīsatikappamhi,",
  "tha-ap64:5.2_0": "sumedho nāma khattiyo;",
  "tha-ap64:5.3_0": "Sattaratanasampanno,",
  "tha-ap64:5.4_0": "cakkavattī mahabbalo.",
  "tha-ap64:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap64:6.2_0": "…pe…",
  "tha-ap64:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap64:7.1_0": "Itthaṁ sudaṁ āyasmā avopupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap64:8.1_0": "Avopupphiyattherassāpadānaṁ dutiyaṁ."
}