{
  "tha-ap25:0.1_0": "Therāpadāna",
  "tha-ap25:0.2_0": "Subhūtivagga",
  "tha-ap25:0.3_0": "3 Tisaraṇagamaniyattheraapadāna",
  "tha-ap25:1.1_0": "“Nagare candavatiyā,",
  "tha-ap25:1.2_0": "mātuupaṭṭhāko ahuṁ;",
  "tha-ap25:1.3_0": "Andhā mātā pitā mayhaṁ,",
  "tha-ap25:1.4_0": "te posemi ahaṁ tadā.",
  "tha-ap25:2.1_0": "Rahogato nisīditvā,",
  "tha-ap25:2.2_0": "evaṁ cintesahaṁ tadā;",
  "tha-ap25:2.3_0": "Posento mātāpitaro,",
  "tha-ap25:2.4_0": "pabbajjaṁ na labhāmahaṁ.",
  "tha-ap25:3.1_0": "Mahandhakārapihitā,",
  "tha-ap25:3.2_0": "tividhaggīhi ḍayhare;",
  "tha-ap25:3.3_0": "Etādise bhave jāte,",
  "tha-ap25:3.4_0": "natthi koci vināyako.",
  "tha-ap25:4.1_0": "Buddho loke samuppanno,",
  "tha-ap25:4.2_0": "dippati dāni sāsanaṁ;",
  "tha-ap25:4.3_0": "Sakkā uddharituṁ attā,",
  "tha-ap25:4.4_0": "puññakāmena jantunā.",
  "tha-ap25:5.1_0": "Uggayha tīṇi saraṇe,",
  "tha-ap25:5.2_0": "paripuṇṇāni gopayiṁ;",
  "tha-ap25:5.3_0": "Tena kammena sukatena,",
  "tha-ap25:5.4_0": "paṭimokkhāmi duggatiṁ.",
  "tha-ap25:6.1_0": "Nisabho nāma samaṇo,",
  "tha-ap25:6.2_0": "buddhassa aggasāvako;",
  "tha-ap25:6.3_0": "Tamahaṁ upagantvāna,",
  "tha-ap25:6.4_0": "saraṇagamanaṁ gahiṁ.",
  "tha-ap25:7.1_0": "Vassasatasahassāni,",
  "tha-ap25:7.2_0": "āyu vijjati tāvade;",
  "tha-ap25:7.3_0": "Tāvatā saraṇagamanaṁ,",
  "tha-ap25:7.4_0": "paripuṇṇaṁ agopayiṁ.",
  "tha-ap25:8.1_0": "Carime vattamānamhi,",
  "tha-ap25:8.2_0": "saraṇaṁ taṁ anussariṁ;",
  "tha-ap25:8.3_0": "Tena kammena sukatena,",
  "tha-ap25:8.4_0": "tāvatiṁsaṁ agacchahaṁ.",
  "tha-ap25:9.1_0": "Devalokagato santo,",
  "tha-ap25:9.2_0": "puññakammasamāhito;",
  "tha-ap25:9.3_0": "Yaṁ desaṁ upapajjāmi,",
  "tha-ap25:9.4_0": "aṭṭha hetū labhāmahaṁ.",
  "tha-ap25:10.1_0": "Disāsu pūjito homi,",
  "tha-ap25:10.2_0": "tikkhapañño bhavāmahaṁ;",
  "tha-ap25:10.3_0": "Sabbe devānuvattanti,",
  "tha-ap25:10.4_0": "amitabhogaṁ labhāmahaṁ.",
  "tha-ap25:11.1_0": "Suvaṇṇavaṇṇo sabbattha,",
  "tha-ap25:11.2_0": "paṭikanto bhavāmahaṁ;",
  "tha-ap25:11.3_0": "Mittānaṁ acalo homi,",
  "tha-ap25:11.4_0": "yaso abbhuggato mamaṁ.",
  "tha-ap25:12.1_0": "Asītikkhattu devindo,",
  "tha-ap25:12.2_0": "devarajjamakārayiṁ;",
  "tha-ap25:12.3_0": "Dibbasukhaṁ anubhaviṁ,",
  "tha-ap25:12.4_0": "accharāhi purakkhato.",
  "tha-ap25:13.1_0": "Pañcasattatikkhattuñca,",
  "tha-ap25:13.2_0": "cakkavattī ahosahaṁ;",
  "tha-ap25:13.3_0": "Padesarajjaṁ vipulaṁ,",
  "tha-ap25:13.4_0": "gaṇanāto asaṅkhiyaṁ.",
  "tha-ap25:14.1_0": "Pacchime bhave sampatte,",
  "tha-ap25:14.2_0": "puññakammasamāhito;",
  "tha-ap25:14.3_0": "Pure sāvatthiyaṁ jāto,",
  "tha-ap25:14.4_0": "mahāsāle suaḍḍhake.",
  "tha-ap25:15.1_0": "Nagarā nikkhamitvāna,",
  "tha-ap25:15.2_0": "dārakehi purakkhato;",
  "tha-ap25:15.3_0": "Hasakhiḍḍasamaṅgīhaṁ,",
  "tha-ap25:15.4_0": "saṅghārāmaṁ upāgamiṁ.",
  "tha-ap25:16.1_0": "Tatthaddasāsiṁ samaṇaṁ,",
  "tha-ap25:16.2_0": "vippamuttaṁ nirūpadhiṁ;",
  "tha-ap25:16.3_0": "So me dhammamadesesi,",
  "tha-ap25:16.4_0": "saraṇañca adāsi me.",
  "tha-ap25:17.1_0": "Sohaṁ sutvāna saraṇaṁ,",
  "tha-ap25:17.2_0": "saraṇaṁ me anussariṁ;",
  "tha-ap25:17.3_0": "Ekāsane nisīditvā,",
  "tha-ap25:17.4_0": "arahattamapāpuṇiṁ.",
  "tha-ap25:18.1_0": "Jātiyā sattame vasse,",
  "tha-ap25:18.2_0": "arahattamapāpuṇiṁ;",
  "tha-ap25:18.3_0": "Upasampādayi buddho,",
  "tha-ap25:18.4_0": "guṇamaññāya cakkhumā.",
  "tha-ap25:19.1_0": "Aparimeyye ito kappe,",
  "tha-ap25:19.2_0": "saraṇāni agacchahaṁ;",
  "tha-ap25:19.3_0": "Tato me sukataṁ kammaṁ,",
  "tha-ap25:19.4_0": "phalaṁ dassesi me idha.",
  "tha-ap25:20.1_0": "Sugopitaṁ me saraṇaṁ,",
  "tha-ap25:20.2_0": "mānasaṁ suppaṇīhitaṁ;",
  "tha-ap25:20.3_0": "Anubhotvā yasaṁ sabbaṁ,",
  "tha-ap25:20.4_0": "pattomhi acalaṁ padaṁ.",
  "tha-ap25:21.1_0": "Yesaṁ sotāvadhānatthi,",
  "tha-ap25:21.2_0": "suṇotha mama bhāsato;",
  "tha-ap25:21.3_0": "Ahaṁ vo kathayissāmi,",
  "tha-ap25:21.4_0": "sāmaṁ diṭṭhaṁ padaṁ mama.",
  "tha-ap25:22.1_0": "‘Buddho loke samuppanno,",
  "tha-ap25:22.2_0": "vattate jinasāsanaṁ;",
  "tha-ap25:22.3_0": "Amatā vāditā bherī,",
  "tha-ap25:22.4_0": "sokasallavinodanā.",
  "tha-ap25:23.1_0": "Yathāsakena thāmena,",
  "tha-ap25:23.2_0": "puññakkhette anuttare;",
  "tha-ap25:23.3_0": "Adhikāraṁ kareyyātha,",
  "tha-ap25:23.4_0": "passayissatha nibbutiṁ.",
  "tha-ap25:24.1_0": "Paggayha tīṇi saraṇe,",
  "tha-ap25:24.2_0": "pañcasīlāni gopiya;",
  "tha-ap25:24.3_0": "Buddhe cittaṁ pasādetvā,",
  "tha-ap25:24.4_0": "dukkhassantaṁ karissatha.",
  "tha-ap25:25.1_0": "Sammā dhammaṁ bhāvetvāna,",
  "tha-ap25:25.2_0": "Sīlāni parigopiya;",
  "tha-ap25:25.3_0": "Aciraṁ arahattaṁ vo,",
  "tha-ap25:25.4_0": "Sabbepi pāpuṇissatha.",
  "tha-ap25:26.1_0": "Tevijjo iddhipattomhi,",
  "tha-ap25:26.2_0": "cetopariyakovido;",
  "tha-ap25:26.3_0": "Sāvako te mahāvīra,",
  "tha-ap25:26.4_0": "saraṇo vandati satthuno’.",
  "tha-ap25:27.1_0": "Aparimeyye ito kappe,",
  "tha-ap25:27.2_0": "saraṇaṁ buddhassa gacchahaṁ;",
  "tha-ap25:27.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap25:27.4_0": "saraṇaṁ gamane phalaṁ.",
  "tha-ap25:28.1_0": "Paṭisambhidā catasso,",
  "tha-ap25:28.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap25:28.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap25:28.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap25:29.1_0": "Itthaṁ sudaṁ āyasmā tisaraṇagamaniyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap25:30.1_0": "Tisaraṇagamaniyattherassāpadānaṁ tatiyaṁ."
}