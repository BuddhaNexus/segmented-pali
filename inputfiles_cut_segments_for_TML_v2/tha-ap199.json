{
  "tha-ap199:0.1_0": "Therāpadāna",
  "tha-ap199:0.2_0": "Tamālapupphiyavagga",
  "tha-ap199:0.3_0": "7. Tindukadāyakattheraapadāna",
  "tha-ap199:1.1_0": "“Giriduggacaro āsiṁ,",
  "tha-ap199:1.2_0": "makkaṭo thāmavegiko;",
  "tha-ap199:1.3_0": "Phalinaṁ tindukaṁ disvā,",
  "tha-ap199:1.4_0": "buddhaseṭṭhaṁ anussariṁ.",
  "tha-ap199:2.1_0": "Nikkhamitvā katipāhaṁ,",
  "tha-ap199:2.2_0": "viciniṁ lokanāyakaṁ;",
  "tha-ap199:2.3_0": "Pasannacitto sumano,",
  "tha-ap199:2.4_0": "siddhatthaṁ tibhavantaguṁ.",
  "tha-ap199:3.1_0": "Mama saṅkappamaññāya,",
  "tha-ap199:3.2_0": "satthā loke anuttaro;",
  "tha-ap199:3.3_0": "Khīṇāsavasahassehi,",
  "tha-ap199:3.4_0": "āgacchi mama santikaṁ.",
  "tha-ap199:4.1_0": "Pāmojjaṁ janayitvāna,",
  "tha-ap199:4.2_0": "phalahattho upāgamiṁ;",
  "tha-ap199:4.3_0": "Paṭiggahesi bhagavā,",
  "tha-ap199:4.4_0": "sabbaññū vadataṁ varo.",
  "tha-ap199:5.1_0": "Catunnavutito kappe,",
  "tha-ap199:5.2_0": "yaṁ phalaṁ adadiṁ tadā;",
  "tha-ap199:5.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap199:5.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap199:6.1_0": "Sattapaññāsakappamhi,",
  "tha-ap199:6.2_0": "upanandasanāmako;",
  "tha-ap199:6.3_0": "Sattaratanasampanno,",
  "tha-ap199:6.4_0": "cakkavattī mahabbalo.",
  "tha-ap199:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap199:7.2_0": "…pe…",
  "tha-ap199:7.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap199:8.1_0": "Itthaṁ sudaṁ āyasmā tindukadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap199:9.1_0": "Tindukadāyakattherassāpadānaṁ sattamaṁ."
}