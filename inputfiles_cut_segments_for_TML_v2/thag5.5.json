{
  "thag5.5:0.1_0": "Theragāthā",
  "thag5.5:0.2_0": "Pañcakanipāta",
  "thag5.5:0.3_0": "Paṭhamavagga",
  "thag5.5:0.4_0": "5.5. Vaḍḍhattheragāthā",
  "thag5.5:1.1_0": "“Sādhū hi kira me mātā,",
  "thag5.5:1.2_0": "Patodaṁ upadaṁsayi;",
  "thag5.5:1.3_0": "Yassāhaṁ vacanaṁ sutvā,",
  "thag5.5:1.4_0": "Anusiṭṭho janettiyā;",
  "thag5.5:1.5_0": "Āraddhavīriyo pahitatto,",
  "thag5.5:1.6_0": "Patto sambodhimuttamaṁ.",
  "thag5.5:2.1_0": "Arahā dakkhiṇeyyomhi,",
  "thag5.5:2.2_0": "tevijjo amataddaso;",
  "thag5.5:2.3_0": "Jetvā namucino senaṁ,",
  "thag5.5:2.4_0": "viharāmi anāsavo.",
  "thag5.5:3.1_0": "Ajjhattañca bahiddhā ca,",
  "thag5.5:3.2_0": "ye me vijjiṁsu āsavā;",
  "thag5.5:3.3_0": "Sabbe asesā ucchinnā,",
  "thag5.5:3.4_0": "na ca uppajjare puna.",
  "thag5.5:4.1_0": "Visāradā kho bhaginī,",
  "thag5.5:4.2_0": "etamatthaṁ abhāsayi;",
  "thag5.5:4.3_0": "‘Apihā nūna mayipi,",
  "thag5.5:4.4_0": "vanatho te na vijjati’.",
  "thag5.5:5.1_0": "Pariyantakataṁ dukkhaṁ,",
  "thag5.5:5.2_0": "antimoyaṁ samussayo;",
  "thag5.5:5.3_0": "Jātimaraṇasaṁsāro,",
  "thag5.5:5.4_0": "natthi dāni punabbhavo”ti.",
  "thag5.5:5.5_0": "… Vaḍḍho thero …."
}