{
  "tha-ap445:0.1_0": "Therāpadāna",
  "tha-ap445:0.2_0": "Vibhītakavagga",
  "tha-ap445:0.3_0": "3 Billiyattheraapadāna",
  "tha-ap445:1.1_0": "“Candabhāgānadītīre,",
  "tha-ap445:1.2_0": "assamo sukato mama;",
  "tha-ap445:1.3_0": "Billarukkhehi ākiṇṇo,",
  "tha-ap445:1.4_0": "nānādumanisevito.",
  "tha-ap445:2.1_0": "Sugandhaṁ beluvaṁ disvā,",
  "tha-ap445:2.2_0": "buddhaseṭṭhamanussariṁ;",
  "tha-ap445:2.3_0": "Khāribhāraṁ pūrayitvā,",
  "tha-ap445:2.4_0": "tuṭṭho saṁviggamānaso.",
  "tha-ap445:3.1_0": "Kakusandhaṁ upāgamma,",
  "tha-ap445:3.2_0": "billapakkamadāsahaṁ;",
  "tha-ap445:3.3_0": "Puññakkhettassa vīrassa,",
  "tha-ap445:3.4_0": "vippasannena cetasā.",
  "tha-ap445:4.1_0": "Imasmiṁyeva kappasmiṁ,",
  "tha-ap445:4.2_0": "yaṁ phalamadadiṁ tadā;",
  "tha-ap445:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap445:4.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap445:5.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap445:5.2_0": "…pe…",
  "tha-ap445:5.3_0": "viharāmi anāsavo.",
  "tha-ap445:6.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap445:6.2_0": "…pe…",
  "tha-ap445:6.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap445:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap445:7.2_0": "…pe…",
  "tha-ap445:7.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap445:8.1_0": "Itthaṁ sudaṁ āyasmā billiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap445:9.1_0": "Billiyattherassāpadānaṁ tatiyaṁ."
}