{
  "kp9:0.1_0": "Khuddakapāṭha",
  "kp9:0.2_0": "9. Mettasutta",
  "kp9:1.1_0": "Karaṇīyamatthakusalena,",
  "kp9:1.2_0": "Yanta santaṁ padaṁ abhisamecca;",
  "kp9:1.3_0": "Sakko ujū ca suhujū ca,",
  "kp9:1.4_0": "Sūvaco cassa mudu anatimānī.",
  "kp9:2.1_0": "Santussako ca subharo ca,",
  "kp9:2.2_0": "Appakicco ca sallahukavutti;",
  "kp9:2.3_0": "Santindriyo ca nipako ca,",
  "kp9:2.4_0": "Appagabbho kulesvananugiddho.",
  "kp9:3.1_0": "Na ca khuddamācare kiñci,",
  "kp9:3.2_0": "Yena viññū pare upavadeyyuṁ;",
  "kp9:3.3_0": "Sukhino va khemino hontu,",
  "kp9:3.4_0": "Sabbasattā bhavantu sukhitattā.",
  "kp9:4.1_0": "Ye keci pāṇabhūtatthi,",
  "kp9:4.2_0": "Tasā vā thāvarā vanavasesā;",
  "kp9:4.3_0": "Dīghā vā ye va mahantā,",
  "kp9:4.4_0": "Majjhimā rassakā aṇukathūlā.",
  "kp9:5.1_0": "Diṭṭhā vā ye va adiṭṭhā,",
  "kp9:5.2_0": "Ye va dūre vasanti avidūre;",
  "kp9:5.3_0": "Bhūtā va sambhavesī va,",
  "kp9:5.4_0": "Sabbasattā bhavantu sukhitattā.",
  "kp9:6.1_0": "Na paro paraṁ nikubbetha,",
  "kp9:6.2_0": "Nātimaññetha katthaci na kañci;",
  "kp9:6.3_0": "Byārosanā paṭighasañña,",
  "kp9:6.4_0": "Nāññamaññassa dukkhamiccheyya.",
  "kp9:7.1_0": "Mātā yathā niyaṁ puttam",
  "kp9:7.2_0": "Āyusā ekaputtamanurakkhe;",
  "kp9:7.3_0": "Evampi sabbabhūtesu,",
  "kp9:7.4_0": "Mānasaṁ bhāvaye aparimāṇaṁ.",
  "kp9:8.1_0": "Mettañca sabbalokasmi,",
  "kp9:8.2_0": "Mānasaṁ bhāvaye aparimāṇaṁ;",
  "kp9:8.3_0": "Uddhaṁ adho ca tiriyañca,",
  "kp9:8.4_0": "Asambādhaṁ averamasapattaṁ.",
  "kp9:9.1_0": "Tiṭṭhaṁ caraṁ nisinno va,",
  "kp9:9.2_0": "Sayāno yāvatāssa vitamiddho;",
  "kp9:9.3_0": "Etaṁ satiṁ adhiṭṭheyya,",
  "kp9:9.4_0": "Brahmametaṁ vihāramidhamāhu.",
  "kp9:10.1_0": "Diṭṭhiñca anupaggamma,",
  "kp9:10.2_0": "Sīlavā dassanena sampanno;",
  "kp9:10.3_0": "Kāmesu vinaya gedhaṁ,",
  "kp9:10.4_0": "Na hi jātuggabbhaseyya punaretīti.",
  "kp9:11.1_0": "Mettasuttaṁ.",
  "kp9:12.1_0": "Khuddakapāṭhapāḷi niṭṭhitā."
}