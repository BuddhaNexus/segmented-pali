{
  "thag1.111:0.1_0": "Theragāthā",
  "thag1.111:0.2_0": "Ekakanipāta",
  "thag1.111:0.3_0": "Dvādasamavagga",
  "thag1.111:0.4_0": "1.111. Jentattheragāthā",
  "thag1.111:1.1_0": "“Duppabbajjaṁ ve duradhivāsā gehā,",
  "thag1.111:1.2_0": "Dhammo gambhīro duradhigamā bhogā;",
  "thag1.111:1.3_0": "Kicchā vutti no itarītareneva,",
  "thag1.111:1.4_0": "Yuttaṁ cintetuṁ satatamaniccatan”ti.",
  "thag1.111:1.5_0": "… Jento thero …."
}