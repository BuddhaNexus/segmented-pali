{
  "snp5.5:0.1_0": "Sutta Nipāta 5.5",
  "snp5.5:0.2_0": "Mettagūmāṇavapucchā",
  "snp5.5:1.1_0": "“Pucchāmi taṁ bhagavā brūhi me taṁ,",
  "snp5.5:1.2_0": "(iccāyasmā mettagū)",
  "snp5.5:1.3_0": "Maññāmi taṁ vedaguṁ bhāvitattaṁ;",
  "snp5.5:1.4_0": "Kuto nu dukkhā samudāgatā ime,",
  "snp5.5:1.5_0": "Ye keci lokasmimanekarūpā”.",
  "snp5.5:2.1_0": "“Dukkhassa ve maṁ pabhavaṁ apucchasi,",
  "snp5.5:2.2_0": "(mettagūti bhagavā)",
  "snp5.5:2.3_0": "Taṁ te pavakkhāmi yathā pajānaṁ;",
  "snp5.5:2.4_0": "Upadhinidānā pabhavanti dukkhā,",
  "snp5.5:2.5_0": "Ye keci lokasmimanekarūpā.",
  "snp5.5:3.1_0": "Yo ve avidvā upadhiṁ karoti,",
  "snp5.5:3.2_0": "Punappunaṁ dukkhamupeti mando;",
  "snp5.5:3.3_0": "Tasmā pajānaṁ upadhiṁ na kayirā,",
  "snp5.5:3.4_0": "Dukkhassa jātippabhavānupassī”.",
  "snp5.5:4.1_0": "“Yaṁ taṁ apucchimha akittayī no,",
  "snp5.5:4.2_0": "Aññaṁ taṁ pucchāma tadiṅgha brūhi;",
  "snp5.5:4.3_0": "Kathaṁ nu dhīrā vitaranti oghaṁ,",
  "snp5.5:4.4_0": "Jātiṁ jaraṁ sokapariddavañca;",
  "snp5.5:4.5_0": "Taṁ me muni sādhu viyākarohi,",
  "snp5.5:4.6_0": "Tathā hi te vidito esa dhammo”.",
  "snp5.5:5.1_0": "“Kittayissāmi te dhammaṁ,",
  "snp5.5:5.2_0": "(mettagūti bhagavā)",
  "snp5.5:5.3_0": "Diṭṭhe dhamme anītihaṁ;",
  "snp5.5:5.4_0": "Yaṁ viditvā sato caraṁ,",
  "snp5.5:5.5_0": "Tare loke visattikaṁ”.",
  "snp5.5:6.1_0": "“Tañcāhaṁ abhinandāmi,",
  "snp5.5:6.2_0": "mahesi dhammamuttamaṁ;",
  "snp5.5:6.3_0": "Yaṁ viditvā sato caraṁ,",
  "snp5.5:6.4_0": "tare loke visattikaṁ”.",
  "snp5.5:7.1_0": "“Yaṁ kiñci sampajānāsi,",
  "snp5.5:7.2_0": "(mettagūti bhagavā)",
  "snp5.5:7.3_0": "Uddhaṁ adho tiriyañcāpi majjhe;",
  "snp5.5:7.4_0": "Etesu nandiñca nivesanañca,",
  "snp5.5:7.5_0": "Panujja viññāṇaṁ bhave na tiṭṭhe.",
  "snp5.5:8.1_0": "Evaṁvihārī sato appamatto,",
  "snp5.5:8.2_0": "Bhikkhu caraṁ hitvā mamāyitāni;",
  "snp5.5:8.3_0": "Jātiṁ jaraṁ sokapariddavañca,",
  "snp5.5:8.4_0": "Idheva vidvā pajaheyya dukkhaṁ”.",
  "snp5.5:9.1_0": "“Etābhinandāmi vaco mahesino,",
  "snp5.5:9.2_0": "Sukittitaṁ gotamanūpadhīkaṁ;",
  "snp5.5:9.3_0": "Addhā hi bhagavā pahāsi dukkhaṁ,",
  "snp5.5:9.4_0": "Tathā hi te vidito esa dhammo.",
  "snp5.5:10.1_0": "Te cāpi nūnappajaheyyu dukkhaṁ,",
  "snp5.5:10.2_0": "Ye tvaṁ muni aṭṭhitaṁ ovadeyya;",
  "snp5.5:10.3_0": "Taṁ taṁ namassāmi samecca nāga,",
  "snp5.5:10.4_0": "Appeva maṁ bhagavā aṭṭhitaṁ ovadeyya”.",
  "snp5.5:11.1_0": "“Yaṁ brāhmaṇaṁ vedagumābhijaññā,",
  "snp5.5:11.2_0": "Akiñcanaṁ kāmabhave asattaṁ;",
  "snp5.5:11.3_0": "Addhā hi so oghamimaṁ atāri,",
  "snp5.5:11.4_0": "Tiṇṇo ca pāraṁ akhilo akaṅkho.",
  "snp5.5:12.1_0": "Vidvā ca yo vedagū naro idha,",
  "snp5.5:12.2_0": "Bhavābhave saṅgamimaṁ visajja;",
  "snp5.5:12.3_0": "So vītataṇho anīgho nirāso,",
  "snp5.5:12.4_0": "Atāri so jātijaranti brūmī”ti.",
  "snp5.5:12.5_0": "Mettagūmāṇavapucchā catutthī."
}