{
  "kp1:0.1_0": "Khuddakapāṭha",
  "kp1:0.2_0": "1. Saraṇattaya",
  "kp1:1.1_0": "Namo tassa Bhagavato Arahato Sammāsambuddhassa.",
  "kp1:2.1_0": "Buddhaṁ saraṇaṁ gacchāmi,",
  "kp1:2.2_0": "Dhammaṁ saraṇaṁ gacchāmi,",
  "kp1:2.3_0": "Saṅghaṁ saraṇaṁ gacchāmi.",
  "kp1:3.1_0": "Dutiyampi buddhaṁ saraṇaṁ gacchāmi,",
  "kp1:3.2_0": "Dutiyampi dhammaṁ saraṇaṁ gacchāmi,",
  "kp1:3.3_0": "Dutiyampi saṅghaṁ saraṇaṁ gacchāmi.",
  "kp1:4.1_0": "Tatiyampi buddhaṁ saraṇaṁ gacchāmi,",
  "kp1:4.2_0": "Tatiyampi dhammaṁ saraṇaṁ gacchāmi,",
  "kp1:4.3_0": "Tatiyampi saṅghaṁ saraṇaṁ gacchāmi.",
  "kp1:5.1_0": "Saraṇattayaṁ."
}