{
  "tika-s0304t8:0_0": "9. Asaṅkhatasaṁyuttaṁ",
  "tika-s0304t8:1_0": "1. Paṭhamavaggo",
  "tika-s0304t8:2_0": "1-11. Kāyagatāsatisuttādivaṇṇanā",
  "tika-s0304t8:3_0": "366-376.Asaṅkhatanti na saṅkhataṁ hetupaccayeti.",
  "tika-s0304t8:3_1": "Tenāha “akata”nti.",
  "tika-s0304t8:3_2": "Hitaṁ esantenāti mettāyantena.",
  "tika-s0304t8:3_3": "Anukampamānenāti karuṇāyantena.",
  "tika-s0304t8:3_4": "Upādāyāti ādiyitvāti ayamatthotiāha “cittena pariggahetvā”ti.",
  "tika-s0304t8:3_5": "Aviparītadhammadesanāti aviparītadhammassa desanā, paṭipattimpi sāvakā viya garuko bhagavā.",
  "tika-s0304t8:3_6": "Dāyajjaṁ attano adhiṭṭhitaṁ niyyāteti.",
  "tika-s0304t8:4_0": "Bhikkhāsampattikālādīnaṁ sattannaṁ sappāyānaṁ sampattiyā labbhanakāle.",
  "tika-s0304t8:4_1": "Vipattikāle pana ettha vuttavipariyāyena attho veditabbo.",
  "tika-s0304t8:4_2": "Bhāriyanti dukkhabahulatāya dāruṇaṁ.",
  "tika-s0304t8:4_3": "Amhākaṁ santikā laddhabbā.",
  "tika-s0304t8:4_4": "Tumhākaṁ anusāsanīti tumhākaṁ dātabbā anusāsanī.",
  "tika-s0304t8:5_0": "Kāyagatāsatisuttādivaṇṇanā niṭṭhitā.",
  "tika-s0304t8:6_0": "2. Dutiyavaggo",
  "tika-s0304t8:7_0": "23-33. Asaṅkhatasuttādivaṇṇanā",
  "tika-s0304t8:8_0": "377-409.",
  "tika-s0304t8:8_1": "Tattha ca natthi ettha taṇhāsaṅkhātaṁ nataṁ, natthi etasmiṁ vā adhigate puggalabhāvoti anataṁ.",
  "tika-s0304t8:8_2": "Anāsavanti etthāpi eseva nayo.",
  "tika-s0304t8:8_3": "Saccadhammatāya saccaṁ.",
  "tika-s0304t8:8_4": "Vaṭṭadukkhato pārametīti pāraṁ.",
  "tika-s0304t8:8_5": "Saṇhaṭṭhenāti sukhumaṭṭhena nipuṇaṁ.",
  "tika-s0304t8:8_6": "Tato eva duddasatāya.",
  "tika-s0304t8:8_7": "Ajajjaraṁ niccasabhāvattā.",
  "tika-s0304t8:8_8": "Natthi etassa nidassananti vā anidassanaṁ.",
  "tika-s0304t8:8_9": "Etasmiṁ adhigate natthi saṁsāre.",
  "tika-s0304t8:8_10": "Papañcanti vā nippapañcaṁ.",
  "tika-s0304t8:9_0": "Etasmiṁ adhigate puggalassa maraṇaṁ natthīti vā amataṁ.",
  "tika-s0304t8:9_1": "Atappakaṭṭhena vā paṇītaṁ.",
  "tika-s0304t8:9_2": "Sukhahetutāya vā sivaṁ.",
  "tika-s0304t8:9_3": "Taṇhā khīyanti etthāti taṇhakkhayaṁ.",
  "tika-s0304t8:10_0": "Aññassa tādisassa abhāvato vimhāpanīyatāya abhūtamevāti.",
  "tika-s0304t8:10_1": "Kutoci paccayato anibbattameva hutvā bhūtaṁ vijjamānaṁ.",
  "tika-s0304t8:10_2": "Tenāha “ajātaṁ hutvā atthī”ti.",
  "tika-s0304t8:10_3": "Natthi ettha dukkhanti niddukkhaṁ, tassa bhāvo niddukkhattaṁ.",
  "tika-s0304t8:10_4": "Tasmā anītikaṁ ītirahitaṁ.",
  "tika-s0304t8:10_5": "Vānaṁ vuccati taṇhā, tadabhāvena nibbānaṁ.",
  "tika-s0304t8:10_6": "Byābajjhaṁ vuccati dukkhaṁ, tadabhāvena abyābajjhaṁ.",
  "tika-s0304t8:10_7": "Paramatthato saccato suddhibhāvena.",
  "tika-s0304t8:10_8": "Kāmā eva puthujjanehi allīyitabbato ālayā.",
  "tika-s0304t8:10_9": "Esa nayo sesesupi.",
  "tika-s0304t8:10_10": "Patiṭṭhaṭṭhenāti patiṭṭhābhāvena vaṭṭadukkhato muccitukāmānaṁ dīpasadisaṁ oghehi anajjhottharaṇīyattā.",
  "tika-s0304t8:10_11": "Allīyitabbayuttaṭṭhenāti allīyituṁ arahabhāvato.",
  "tika-s0304t8:10_12": "Tāyanaṭṭhenāti saparatāyanaṭṭhena.",
  "tika-s0304t8:10_13": "Bhayasaraṇaṭṭhenāti bhayassa hiṁsanaṭṭhena.",
  "tika-s0304t8:10_14": "Seṭṭhaṁ uttamaṁ.",
  "tika-s0304t8:10_15": "Gatīti gandhabbaṭṭhānaṁ.",
  "tika-s0304t8:11_0": "Asaṅkhatasuttādivaṇṇanā niṭṭhitā.",
  "tika-s0304t8:12_0": "Asaṅkhatasaṁyuttavaṇṇanā niṭṭhitā."
}