{
  "vv35:0.1_0": "Vimānavatthu",
  "vv35:0.2_0": "Itthivimāna",
  "vv35:0.3_0": "Pāricchattakavagga",
  "vv35:0.4_0": "7. Pesavatīvimānavatthu",
  "vv35:1.1_0": "“Phalikarajatahemajālachannaṁ,",
  "vv35:1.2_0": "Vividhacitratalamaddasaṁ surammaṁ;",
  "vv35:1.3_0": "Byamhaṁ sunimmitaṁ toraṇūpapannaṁ,",
  "vv35:1.4_0": "Rucakupakiṇṇamidaṁ subhaṁ vimānaṁ.",
  "vv35:2.1_0": "Bhāti ca dasa disā nabheva suriyo,",
  "vv35:2.2_0": "Sarade tamonudo sahassaraṁsī;",
  "vv35:2.3_0": "Tathā tapati midaṁ tava vimānaṁ,",
  "vv35:2.4_0": "Jalamiva dhūmasikho nise nabhagge.",
  "vv35:3.1_0": "Musatīva nayanaṁ sateratāva,",
  "vv35:3.2_0": "Ākāse ṭhapitamidaṁ manuññaṁ;",
  "vv35:3.3_0": "Vīṇāmurajasammatāḷaghuṭṭhaṁ,",
  "vv35:3.4_0": "Iddhaṁ indapuraṁ yathā tavedaṁ.",
  "vv35:4.1_0": "Padumakumuduppalakuvalayaṁ,",
  "vv35:4.2_0": "Yodhika bandhukanojakā ca santi;",
  "vv35:4.3_0": "Sālakusumitapupphitā asokā,",
  "vv35:4.4_0": "Vividhadumaggasugandhasevitamidaṁ.",
  "vv35:5.1_0": "Saḷalalabujabhujaka saṁyuttā,",
  "vv35:5.2_0": "Kusakasuphullitalatāvalambinīhi;",
  "vv35:5.3_0": "Maṇijālasadisā yasassinī,",
  "vv35:5.4_0": "Rammā pokkharaṇī upaṭṭhitā te.",
  "vv35:6.1_0": "Udakaruhā ca yetthi pupphajātā,",
  "vv35:6.2_0": "Thalajā ye ca santi rukkhajātā;",
  "vv35:6.3_0": "Mānusakāmānussakā ca dibbā,",
  "vv35:6.4_0": "Sabbe tuyhaṁ nivesanamhi jātā.",
  "vv35:7.1_0": "Kissa saṁyamadamassayaṁ vipāko,",
  "vv35:7.2_0": "Kenāsi kammaphalenidhūpapannā;",
  "vv35:7.3_0": "Yathā ca te adhigatamidaṁ vimānaṁ,",
  "vv35:7.4_0": "Tadanupadaṁ avacāsiḷārapamhe”ti.",
  "vv35:8.1_0": "“Yathā ca me adhigatamidaṁ vimānaṁ,",
  "vv35:8.2_0": "Koñcamayūracakorasaṅghacaritaṁ;",
  "vv35:8.3_0": "Dibyapilavahaṁsarājaciṇṇaṁ,",
  "vv35:8.4_0": "Dijakāraṇḍavakokilābhinaditaṁ.",
  "vv35:9.1_0": "Nānāsantānakapuppharukkhavividhā,",
  "vv35:9.2_0": "Pāṭalijambuasokarukkhavantaṁ;",
  "vv35:9.3_0": "Yathā ca me adhigatamidaṁ vimānaṁ,",
  "vv35:9.4_0": "Taṁ te pavedayāmi suṇohi bhante.",
  "vv35:10.1_0": "Magadhavarapuratthimena,",
  "vv35:10.2_0": "Nāḷakagāmo nāma atthi bhante;",
  "vv35:10.3_0": "Tattha ahosiṁ pure suṇisā,",
  "vv35:10.4_0": "Pesavatīti tattha jāniṁsu mamaṁ.",
  "vv35:11.1_0": "Sāhamapacitatthadhammakusalaṁ,",
  "vv35:11.2_0": "Devamanussapūjitaṁ mahantaṁ;",
  "vv35:11.3_0": "Upatissaṁ nibbutamappameyyaṁ,",
  "vv35:11.4_0": "Muditamanā kusumehi abbhukiriṁ.",
  "vv35:12.1_0": "Paramagatigatañca pūjayitvā,",
  "vv35:12.2_0": "Antimadehadharaṁ isiṁ uḷāraṁ;",
  "vv35:12.3_0": "Pahāya mānusakaṁ samussayaṁ,",
  "vv35:12.4_0": "Tidasagatā idha māvasāmi ṭhānan”ti.",
  "vv35:13.1_0": "Pesavatīvimānaṁ sattamaṁ."
}