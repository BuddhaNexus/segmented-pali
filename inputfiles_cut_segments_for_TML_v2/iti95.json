{
  "iti95:0.1_0": "Itivuttaka 95",
  "iti95:0.2_0": "Tikanipāta",
  "iti95:0.3_0": "Pañcamavagga",
  "iti95:0.4_0": "Kāmūpapattisutta",
  "iti95:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti95:2.1_0": "“Tisso imā, bhikkhave, kāmūpapattiyo.",
  "iti95:2.2_0": "Katamā tisso?",
  "iti95:2.3_0": "Paccupaṭṭhitakāmā, nimmānaratino, paranimmitavasavattino—",
  "iti95:2.4_0": "imā kho, bhikkhave, tisso kāmūpapattiyo”ti.",
  "iti95:2.5_0": "Etamatthaṁ bhagavā avoca.",
  "iti95:2.6_0": "Tatthetaṁ iti vuccati:",
  "iti95:3.1_0": "“Paccupaṭṭhitakāmā ca,",
  "iti95:3.2_0": "ye devā vasavattino;",
  "iti95:3.3_0": "Nimmānaratino devā,",
  "iti95:3.4_0": "ye caññe kāmabhogino;",
  "iti95:3.5_0": "Itthabhāvaññathābhāvaṁ,",
  "iti95:3.6_0": "saṁsāraṁ nātivattare.",
  "iti95:4.1_0": "Etamādīnavaṁ ñatvā,",
  "iti95:4.2_0": "kāmabhogesu paṇḍito;",
  "iti95:4.3_0": "Sabbe pariccaje kāme,",
  "iti95:4.4_0": "ye dibbā ye ca mānusā.",
  "iti95:5.1_0": "Piyarūpasātagadhitaṁ,",
  "iti95:5.2_0": "chetvā sotaṁ duraccayaṁ;",
  "iti95:5.3_0": "Asesaṁ parinibbanti,",
  "iti95:5.4_0": "asesaṁ dukkhamaccaguṁ.",
  "iti95:6.1_0": "Ariyaddasā vedaguno,",
  "iti95:6.2_0": "sammadaññāya paṇḍitā;",
  "iti95:6.3_0": "Jātikkhayamabhiññāya,",
  "iti95:6.4_0": "nāgacchanti punabbhavan”ti.",
  "iti95:7.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti95:7.2_0": "Chaṭṭhaṁ."
}