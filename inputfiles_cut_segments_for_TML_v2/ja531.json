{
  "ja531:0.1_0": "Jātaka",
  "ja531:0.2_0": "Sattatinipāta",
  "ja531:0.3_0": "Kusavagga",
  "ja531:0.4_0": "1. Kusajātaka",
  "ja531:1.1_0": "“Idaṁ te raṭṭhaṁ sadhanaṁ sayoggaṁ,",
  "ja531:1.2_0": "Sakāyuraṁ sabbakāmūpapannaṁ;",
  "ja531:1.3_0": "Idaṁ te rajjaṁ anusāsa amma,",
  "ja531:1.4_0": "Gacchāmahaṁ yattha piyā pabhāvatī”.",
  "ja531:2.1_0": "“Anujjubhūtena haraṁ mahantaṁ,",
  "ja531:2.2_0": "Divā ca ratto ca nisīthakāle;",
  "ja531:2.3_0": "Paṭigaccha tvaṁ khippaṁ kusāvatiṁ kusa,",
  "ja531:2.4_0": "Nicchāmi dubbaṇṇamahaṁ vasantaṁ”.",
  "ja531:3.1_0": "“Nāhaṁ gamissāmi ito kusāvatiṁ,",
  "ja531:3.2_0": "Pabhāvatī vaṇṇapalobhito tava;",
  "ja531:3.3_0": "Ramāmi maddassa niketaramme,",
  "ja531:3.4_0": "Hitvāna raṭṭhaṁ tava dassane rato.",
  "ja531:4.1_0": "Pabhāvatī vaṇṇapalobhito tava,",
  "ja531:4.2_0": "Sammūḷharūpo vicarāmi mediniṁ;",
  "ja531:4.3_0": "Disaṁ na jānāmi kutomhi āgato,",
  "ja531:4.4_0": "Tayamhi matto migamandalocane.",
  "ja531:5.1_0": "Suvaṇṇacīravasane,",
  "ja531:5.2_0": "jātarūpasumekhale;",
  "ja531:5.3_0": "Sussoṇi tava kāmā hi,",
  "ja531:5.4_0": "nāhaṁ rajjena matthiko”.",
  "ja531:6.1_0": "“Abbhūti tassa bho hoti,",
  "ja531:6.2_0": "yo anicchantamicchati;",
  "ja531:6.3_0": "Akāmaṁ rāja kāmesi,",
  "ja531:6.4_0": "akantaṁ kantumicchasi”.",
  "ja531:7.1_0": "“Akāmaṁ vā sakāmaṁ vā,",
  "ja531:7.2_0": "yo naro labhate piyaṁ;",
  "ja531:7.3_0": "Lābhamettha pasaṁsāma,",
  "ja531:7.4_0": "alābho tattha pāpako”.",
  "ja531:8.1_0": "“Pāsāṇasāraṁ khaṇasi,",
  "ja531:8.2_0": "kaṇikārassa dārunā;",
  "ja531:8.3_0": "Vātaṁ jālena bādhesi,",
  "ja531:8.4_0": "yo anicchantamicchasi”.",
  "ja531:9.1_0": "“Pāsāṇo nūna te hadaye,",
  "ja531:9.2_0": "ohito mudulakkhaṇe;",
  "ja531:9.3_0": "Yo te sātaṁ na vindāmi,",
  "ja531:9.4_0": "tirojanapadāgato.",
  "ja531:10.1_0": "Yadā maṁ bhakuṭiṁ katvā,",
  "ja531:10.2_0": "rājaputtī udikkhati;",
  "ja531:10.3_0": "Āḷāriko tadā homi,",
  "ja531:10.4_0": "rañño maddassantepure.",
  "ja531:11.1_0": "Yadā umhayamānā maṁ,",
  "ja531:11.2_0": "rājaputtī udikkhati;",
  "ja531:11.3_0": "Nāḷāriko tadā homi,",
  "ja531:11.4_0": "rājā homi tadā kuso”.",
  "ja531:12.1_0": "“Sace hi vacanaṁ saccaṁ,",
  "ja531:12.2_0": "nemittānaṁ bhavissati;",
  "ja531:12.3_0": "Neva me tvaṁ patī assa,",
  "ja531:12.4_0": "kāmaṁ chindantu sattadhā”.",
  "ja531:13.1_0": "“Sace hi vacanaṁ saccaṁ,",
  "ja531:13.2_0": "aññesaṁ yadi vā mamaṁ;",
  "ja531:13.3_0": "Neva tuyhaṁ patī atthi,",
  "ja531:13.4_0": "añño sīhassarā kusā”.",
  "ja531:14.1_0": "“Nekkhaṁ gīvaṁ te kāressaṁ,",
  "ja531:14.2_0": "patvā khujje kusāvatiṁ;",
  "ja531:14.3_0": "Sace maṁ nāganāsūrū,",
  "ja531:14.4_0": "olokeyya pabhāvatī.",
  "ja531:15.1_0": "Nekkhaṁ gīvaṁ te kāressaṁ,",
  "ja531:15.2_0": "patvā khujje kusāvatiṁ;",
  "ja531:15.3_0": "Sace maṁ nāganāsūrū,",
  "ja531:15.4_0": "ālapeyya pabhāvatī.",
  "ja531:16.1_0": "Nekkhaṁ gīvaṁ te kāressaṁ,",
  "ja531:16.2_0": "patvā khujje kusāvatiṁ;",
  "ja531:16.3_0": "Sace maṁ nāganāsūrū,",
  "ja531:16.4_0": "umhāyeyya pabhāvatī.",
  "ja531:17.1_0": "Nekkhaṁ gīvaṁ te kāressaṁ,",
  "ja531:17.2_0": "patvā khujje kusāvatiṁ;",
  "ja531:17.3_0": "Sace maṁ nāganāsūrū,",
  "ja531:17.4_0": "pamhāyeyya pabhāvatī.",
  "ja531:18.1_0": "Nekkhaṁ gīvaṁ te kāressaṁ,",
  "ja531:18.2_0": "patvā khujje kusāvatiṁ;",
  "ja531:18.3_0": "Sace maṁ nāganāsūrū,",
  "ja531:18.4_0": "pāṇīhi upasamphuse”.",
  "ja531:19.1_0": "“Na hi nūnāyaṁ rājaputtī,",
  "ja531:19.2_0": "kuse sātampi vindati;",
  "ja531:19.3_0": "Āḷārike bhate pose,",
  "ja531:19.4_0": "vetanena anatthike”.",
  "ja531:20.1_0": "“Na hi nūnāyaṁ sā khujjā,",
  "ja531:20.2_0": "labhati jivhāya chedanaṁ;",
  "ja531:20.3_0": "Sunisitena satthena,",
  "ja531:20.4_0": "evaṁ dubbhāsitaṁ bhaṇaṁ”.",
  "ja531:21.1_0": "“Mā naṁ rūpena pāmesi,",
  "ja531:21.2_0": "ārohena pabhāvati;",
  "ja531:21.3_0": "Mahāyasoti katvāna,",
  "ja531:21.4_0": "karassu rucire piyaṁ.",
  "ja531:22.1_0": "Mā naṁ rūpena pāmesi,",
  "ja531:22.2_0": "ārohena pabhāvati;",
  "ja531:22.3_0": "Mahaddhanoti katvāna,",
  "ja531:22.4_0": "karassu rucire piyaṁ.",
  "ja531:23.1_0": "Mā naṁ rūpena pāmesi,",
  "ja531:23.2_0": "ārohena pabhāvati;",
  "ja531:23.3_0": "Mahabbaloti katvāna,",
  "ja531:23.4_0": "karassu rucire piyaṁ.",
  "ja531:24.1_0": "Mā naṁ rūpena pāmesi,",
  "ja531:24.2_0": "ārohena pabhāvati;",
  "ja531:24.3_0": "Mahāraṭṭhoti katvāna,",
  "ja531:24.4_0": "karassu rucire piyaṁ.",
  "ja531:25.1_0": "Mā naṁ rūpena pāmesi,",
  "ja531:25.2_0": "ārohena pabhāvati;",
  "ja531:25.3_0": "Mahārājāti katvāna,",
  "ja531:25.4_0": "karassu rucire piyaṁ.",
  "ja531:26.1_0": "Mā naṁ rūpena pāmesi,",
  "ja531:26.2_0": "ārohena pabhāvati;",
  "ja531:26.3_0": "Sīhassaroti katvāna,",
  "ja531:26.4_0": "karassu rucire piyaṁ.",
  "ja531:27.1_0": "Mā naṁ rūpena pāmesi,",
  "ja531:27.2_0": "ārohena pabhāvati;",
  "ja531:27.3_0": "Vaggussaroti katvāna,",
  "ja531:27.4_0": "karassu rucire piyaṁ.",
  "ja531:28.1_0": "Mā naṁ rūpena pāmesi,",
  "ja531:28.2_0": "ārohena pabhāvati;",
  "ja531:28.3_0": "Bindussaroti katvāna,",
  "ja531:28.4_0": "karassu rucire piyaṁ.",
  "ja531:29.1_0": "Mā naṁ rūpena pāmesi,",
  "ja531:29.2_0": "ārohena pabhāvati;",
  "ja531:29.3_0": "Mañjussaroti katvāna,",
  "ja531:29.4_0": "karassu rucire piyaṁ.",
  "ja531:30.1_0": "Mā naṁ rūpena pāmesi,",
  "ja531:30.2_0": "ārohena pabhāvati;",
  "ja531:30.3_0": "Madhussaroti katvāna,",
  "ja531:30.4_0": "karassu rucire piyaṁ.",
  "ja531:31.1_0": "Mā naṁ rūpena pāmesi,",
  "ja531:31.2_0": "ārohena pabhāvati;",
  "ja531:31.3_0": "Satasippoti katvāna,",
  "ja531:31.4_0": "karassu rucire piyaṁ.",
  "ja531:32.1_0": "Mā naṁ rūpena pāmesi,",
  "ja531:32.2_0": "ārohena pabhāvati;",
  "ja531:32.3_0": "Khattiyotipi katvāna,",
  "ja531:32.4_0": "karassu rucire piyaṁ.",
  "ja531:33.1_0": "Mā naṁ rūpena pāmesi,",
  "ja531:33.2_0": "ārohena pabhāvati;",
  "ja531:33.3_0": "Kusarājāti katvāna,",
  "ja531:33.4_0": "karassu rucire piyaṁ”.",
  "ja531:34.1_0": "“Ete nāgā upatthaddhā,",
  "ja531:34.2_0": "sabbe tiṭṭhanti vammitā;",
  "ja531:34.3_0": "Purā maddanti pākāraṁ,",
  "ja531:34.4_0": "ānentetaṁ pabhāvatiṁ”.",
  "ja531:35.1_0": "“Satta bile karitvāna,",
  "ja531:35.2_0": "ahametaṁ pabhāvatiṁ;",
  "ja531:35.3_0": "Khattiyānaṁ padassāmi,",
  "ja531:35.4_0": "ye maṁ hantuṁ idhāgatā”.",
  "ja531:36.1_0": "“Avuṭṭhahi rājaputtī,",
  "ja531:36.2_0": "sāmā koseyyavāsinī;",
  "ja531:36.3_0": "Assupuṇṇehi nettehi,",
  "ja531:36.4_0": "dāsīgaṇapurakkhatā”.",
  "ja531:37.1_0": "“Taṁ nūna kakkūpanisevitaṁ mukhaṁ,",
  "ja531:37.2_0": "Ādāsadantātharupaccavekkhitaṁ;",
  "ja531:37.3_0": "Subhaṁ sunettaṁ virajaṁ anaṅgaṇaṁ,",
  "ja531:37.4_0": "Chuddhaṁ vane ṭhassati khattiyehi.",
  "ja531:38.1_0": "Te nūna me asite vellitagge,",
  "ja531:38.2_0": "Kese mudū candanasāralitte;",
  "ja531:38.3_0": "Samākule sīvathikāya majjhe,",
  "ja531:38.4_0": "Pādehi gijjhā parikaḍḍhissanti.",
  "ja531:39.1_0": "Tā nūna me tambanakhā sulomā,",
  "ja531:39.2_0": "Bāhā mudū candanasāralittā;",
  "ja531:39.3_0": "Chinnā vane ujjhitā khattiyehi,",
  "ja531:39.4_0": "Gayha dhaṅko gacchati yenakāmaṁ.",
  "ja531:40.1_0": "Te nūna tālūpanibhe alambe,",
  "ja531:40.2_0": "Nisevite kāsikacandanena;",
  "ja531:40.3_0": "Thanesu me lambissati siṅgālo,",
  "ja531:40.4_0": "Mātūva putto taruṇo tanūjo.",
  "ja531:41.1_0": "Taṁ nūna soṇiṁ puthulaṁ sukoṭṭitaṁ,",
  "ja531:41.2_0": "Nisevitaṁ kañcanamekhalāhi;",
  "ja531:41.3_0": "Chinnaṁ vane khattiyehī avatthaṁ,",
  "ja531:41.4_0": "Siṅgālasaṅghā parikaḍḍhissanti.",
  "ja531:42.1_0": "Soṇā dhaṅkā siṅgālā ca,",
  "ja531:42.2_0": "ye caññe santi dāṭhino;",
  "ja531:42.3_0": "Ajarā nūna hessanti,",
  "ja531:42.4_0": "bhakkhayitvā pabhāvatiṁ.",
  "ja531:43.1_0": "Sace maṁsāni hariṁsu,",
  "ja531:43.2_0": "khattiyā dūragāmino;",
  "ja531:43.3_0": "Aṭṭhīni amma yācitvā,",
  "ja531:43.4_0": "anupathe dahātha naṁ.",
  "ja531:44.1_0": "Khettāni amma kāretvā,",
  "ja531:44.2_0": "kaṇikārettha ropaya;",
  "ja531:44.3_0": "Yadā te pupphitā assu,",
  "ja531:44.4_0": "hemantānaṁ himaccaye;",
  "ja531:44.5_0": "Sareyyātha mamaṁ amma,",
  "ja531:44.6_0": "evaṁvaṇṇā pabhāvatī”.",
  "ja531:45.1_0": "“Tassā mātā udaṭṭhāsi,",
  "ja531:45.2_0": "khattiyā devavaṇṇinī;",
  "ja531:45.3_0": "Disvā asiñca sūnañca,",
  "ja531:45.4_0": "rañño maddassantepure”.",
  "ja531:46.1_0": "“Iminā nūna asinā,",
  "ja531:46.2_0": "susaññaṁ tanumajjhimaṁ;",
  "ja531:46.3_0": "Dhītaraṁ madda hantvāna,",
  "ja531:46.4_0": "khattiyānaṁ padassasi.",
  "ja531:47.1_0": "Na me akāsi vacanaṁ,",
  "ja531:47.2_0": "atthakāmāya puttike;",
  "ja531:47.3_0": "Sājja lohitasañchannā,",
  "ja531:47.4_0": "gacchasi yamasādhanaṁ.",
  "ja531:48.1_0": "Evamāpajjatī poso,",
  "ja531:48.2_0": "pāpiyañca nigacchati;",
  "ja531:48.3_0": "Yo ve hitānaṁ vacanaṁ,",
  "ja531:48.4_0": "na karoti atthadassinaṁ.",
  "ja531:49.1_0": "Sace ca ajja dhāresi,",
  "ja531:49.2_0": "kumāraṁ cārudassanaṁ;",
  "ja531:49.3_0": "Kusena jātaṁ khattiyaṁ,",
  "ja531:49.4_0": "suvaṇṇamaṇimekhalaṁ;",
  "ja531:49.5_0": "Pūjitaṁ ñātisaṅghehi,",
  "ja531:49.6_0": "na gacchasi yamakkhayaṁ.",
  "ja531:50.1_0": "Yatthassu bherī nadati,",
  "ja531:50.2_0": "kuñjaro ca nikūjati;",
  "ja531:50.3_0": "Khattiyānaṁ kule bhadde,",
  "ja531:50.4_0": "kiṁ nu sukhataraṁ tato.",
  "ja531:51.1_0": "Asso ca sisati dvāre,",
  "ja531:51.2_0": "kumāro uparodati;",
  "ja531:51.3_0": "Khattiyānaṁ kule bhadde,",
  "ja531:51.4_0": "kiṁ nu sukhataraṁ tato.",
  "ja531:52.1_0": "Mayūrakoñcābhirude,",
  "ja531:52.2_0": "kokilābhinikūjite;",
  "ja531:52.3_0": "Khattiyānaṁ kule bhadde,",
  "ja531:52.4_0": "kiṁ nu sukhataraṁ tato”.",
  "ja531:53.1_0": "“Kahaṁ nu so sattumaddano,",
  "ja531:53.2_0": "pararaṭṭhappamaddano;",
  "ja531:53.3_0": "Kuso soḷārapaññāṇo,",
  "ja531:53.4_0": "yo no dukkhā pamocaye”.",
  "ja531:54.1_0": "“Idheva so sattumaddano,",
  "ja531:54.2_0": "pararaṭṭhappamaddano;",
  "ja531:54.3_0": "Kuso soḷārapaññāṇo,",
  "ja531:54.4_0": "yo te sabbe vadhissati”.",
  "ja531:55.1_0": "“Ummattikā nu bhaṇasi,",
  "ja531:55.2_0": "Andhabālā pabhāsasi;",
  "ja531:55.3_0": "Kuso ce āgato assa,",
  "ja531:55.4_0": "Kiṁ na jānemu taṁ mayaṁ”.",
  "ja531:56.1_0": "“Eso āḷāriko poso,",
  "ja531:56.2_0": "kumārīpuramantare;",
  "ja531:56.3_0": "Daḷhaṁ katvāna saṁvelliṁ,",
  "ja531:56.4_0": "kumbhiṁ dhovati oṇato”.",
  "ja531:57.1_0": "“Veṇī tvamasi caṇḍālī,",
  "ja531:57.2_0": "adūsi kulagandhinī;",
  "ja531:57.3_0": "Kathaṁ maddakule jātā,",
  "ja531:57.4_0": "dāsaṁ kayirāsi kāmukaṁ”.",
  "ja531:58.1_0": "“Namhi veṇī na caṇḍālī,",
  "ja531:58.2_0": "na camhi kulagandhinī;",
  "ja531:58.3_0": "Okkākaputto bhaddante,",
  "ja531:58.4_0": "tvaṁ nu dāsoti maññasi”.",
  "ja531:59.1_0": "“Yo brāhmaṇasahassāni,",
  "ja531:59.2_0": "sadā bhojeti vīsatiṁ;",
  "ja531:59.3_0": "Okkākaputto bhaddante,",
  "ja531:59.4_0": "tvaṁ nu dāsoti maññasi.",
  "ja531:60.1_0": "Yassa nāgasahassāni,",
  "ja531:60.2_0": "sadā yojenti vīsatiṁ;",
  "ja531:60.3_0": "Okkākaputto bhaddante,",
  "ja531:60.4_0": "tvaṁ nu dāsoti maññasi.",
  "ja531:61.1_0": "Yassa assasahassāni,",
  "ja531:61.2_0": "sadā yojenti vīsatiṁ;",
  "ja531:61.3_0": "Okkākaputto bhaddante,",
  "ja531:61.4_0": "tvaṁ nu dāsoti maññasi.",
  "ja531:62.1_0": "Yassa rathasahassāni,",
  "ja531:62.2_0": "sadā yojenti vīsatiṁ;",
  "ja531:62.3_0": "Okkākaputto bhaddante,",
  "ja531:62.4_0": "tvaṁ nu dāsoti maññasi.",
  "ja531:63.1_0": "Yassa usabhasahassāni,",
  "ja531:63.2_0": "sadā yojenti vīsatiṁ;",
  "ja531:63.3_0": "Okkākaputto bhaddante,",
  "ja531:63.4_0": "tvaṁ nu dāsoti maññasi.",
  "ja531:64.1_0": "Yassa dhenusahassāni,",
  "ja531:64.2_0": "sadā duhanti vīsatiṁ;",
  "ja531:64.3_0": "Okkākaputto bhaddante,",
  "ja531:64.4_0": "tvaṁ nu dāsoti maññasi”.",
  "ja531:65.1_0": "“Taggha te dukkaṭaṁ bāle,",
  "ja531:65.2_0": "yaṁ khattiyaṁ mahabbalaṁ;",
  "ja531:65.3_0": "Nāgaṁ maṇḍūkavaṇṇena,",
  "ja531:65.4_0": "na naṁ akkhāsidhāgataṁ”.",
  "ja531:66.1_0": "“Aparādhaṁ mahārāja,",
  "ja531:66.2_0": "tvaṁ no khama rathesabha;",
  "ja531:66.3_0": "Yaṁ taṁ aññātavesena,",
  "ja531:66.4_0": "nāññāsimhā idhāgataṁ”.",
  "ja531:67.1_0": "“Mādisassa na taṁ channaṁ,",
  "ja531:67.2_0": "yohaṁ āḷāriko bhave;",
  "ja531:67.3_0": "Tvaññeva me pasīdassu,",
  "ja531:67.4_0": "natthi te deva dukkaṭaṁ”.",
  "ja531:68.1_0": "“Gaccha bāle khamāpehi,",
  "ja531:68.2_0": "kusarājaṁ mahabbalaṁ;",
  "ja531:68.3_0": "Khamāpito kuso rājā,",
  "ja531:68.4_0": "so te dassati jīvitaṁ”.",
  "ja531:69.1_0": "“Pitussa vacanaṁ sutvā,",
  "ja531:69.2_0": "devavaṇṇī pabhāvatī;",
  "ja531:69.3_0": "Sirasā aggahī pāde,",
  "ja531:69.4_0": "kusarājaṁ mahabbalaṁ”.",
  "ja531:70.1_0": "“Yāmā ratyo atikkantā,",
  "ja531:70.2_0": "tāmā deva tayā vinā;",
  "ja531:70.3_0": "Vande te sirasā pāde,",
  "ja531:70.4_0": "mā me kujjha rathesabha.",
  "ja531:71.1_0": "Sabbaṁ te paṭijānāmi,",
  "ja531:71.2_0": "mahārāja suṇohi me;",
  "ja531:71.3_0": "Na cāpi appiyaṁ tuyhaṁ,",
  "ja531:71.4_0": "kareyyāmi ahaṁ puna.",
  "ja531:72.1_0": "Evañce yācamānāya,",
  "ja531:72.2_0": "vacanaṁ me na kāhasi;",
  "ja531:72.3_0": "Idāni maṁ tāto hantvā,",
  "ja531:72.4_0": "khattiyānaṁ padassati”.",
  "ja531:73.1_0": "“Evaṁ te yācamānāya,",
  "ja531:73.2_0": "kiṁ na kāhāmi te vaco;",
  "ja531:73.3_0": "Vikuddho tyasmi kalyāṇi,",
  "ja531:73.4_0": "mā tvaṁ bhāyi pabhāvati.",
  "ja531:74.1_0": "Sabbaṁ te paṭijānāmi,",
  "ja531:74.2_0": "rājaputti suṇohi me;",
  "ja531:74.3_0": "Na cāpi appiyaṁ tuyhaṁ,",
  "ja531:74.4_0": "kareyyāmi ahaṁ puna.",
  "ja531:75.1_0": "Tava kāmā hi sussoṇi,",
  "ja531:75.2_0": "pahu dukkhaṁ titikkhisaṁ;",
  "ja531:75.3_0": "Bahuṁ maddakulaṁ hantvā,",
  "ja531:75.4_0": "nayituṁ taṁ pabhāvati”.",
  "ja531:76.1_0": "“Yojayantu rathe asse,",
  "ja531:76.2_0": "nānācitte samāhite;",
  "ja531:76.3_0": "Atha dakkhatha me vegaṁ,",
  "ja531:76.4_0": "vidhamantassa sattavo”.",
  "ja531:77.1_0": "“Tañca tattha udikkhiṁsu,",
  "ja531:77.2_0": "rañño maddassantepure;",
  "ja531:77.3_0": "Vijambhamānaṁ sīhaṁva,",
  "ja531:77.4_0": "phoṭentaṁ diguṇaṁ bhujaṁ.",
  "ja531:78.1_0": "Hatthikkhandhañca āruyha,",
  "ja531:78.2_0": "āropetvā pabhāvatiṁ;",
  "ja531:78.3_0": "Saṅgāmaṁ otaritvāna,",
  "ja531:78.4_0": "sīhanādaṁ nadī kuso.",
  "ja531:79.1_0": "Tassa taṁ nadato sutvā,",
  "ja531:79.2_0": "sīhassevitare migā;",
  "ja531:79.3_0": "Khattiyā vipalāyiṁsu,",
  "ja531:79.4_0": "kusasaddabhayaṭṭitā.",
  "ja531:80.1_0": "Hatthārohā anīkaṭṭhā,",
  "ja531:80.2_0": "rathikā pattikārakā;",
  "ja531:80.3_0": "Aññamaññassa chindanti,",
  "ja531:80.4_0": "kusasaddabhayaṭṭitā.",
  "ja531:81.1_0": "Tasmiṁ saṅgāmasīsasmiṁ,",
  "ja531:81.2_0": "passitvā haṭṭhamānaso;",
  "ja531:81.3_0": "Kusassa rañño devindo,",
  "ja531:81.4_0": "adā verocanaṁ maṇiṁ.",
  "ja531:82.1_0": "So taṁ vijitvā saṅgāmaṁ,",
  "ja531:82.2_0": "laddhā verocanaṁ maṇiṁ;",
  "ja531:82.3_0": "Hatthikkhandhagato rājā,",
  "ja531:82.4_0": "pāvekkhi nagaraṁ puraṁ.",
  "ja531:83.1_0": "Jīvaggāhaṁ gahetvāna,",
  "ja531:83.2_0": "bandhitvā satta khattiye;",
  "ja531:83.3_0": "Sasurassupanāmesi,",
  "ja531:83.4_0": "ime te deva sattavo.",
  "ja531:84.1_0": "Sabbeva te vasaṁ gatā,",
  "ja531:84.2_0": "amittā vihatā tava;",
  "ja531:84.3_0": "Kāmaṁ karohi te tayā,",
  "ja531:84.4_0": "muñca vā te hanassu vā”.",
  "ja531:85.1_0": "“Tuyheva sattavo ete,",
  "ja531:85.2_0": "na hi te mayha sattavo;",
  "ja531:85.3_0": "Tvaññeva no mahārāja,",
  "ja531:85.4_0": "muñca vā te hanassu vā”.",
  "ja531:86.1_0": "“Imā te dhītaro satta,",
  "ja531:86.2_0": "devakaññūpamā subhā;",
  "ja531:86.3_0": "Dadāhi nesaṁ ekekaṁ,",
  "ja531:86.4_0": "hontu jāmātaro tava”.",
  "ja531:87.1_0": "“Amhākañceva tāsañca,",
  "ja531:87.2_0": "tvaṁ no sabbesamissaro;",
  "ja531:87.3_0": "Tvaññeva no mahārāja,",
  "ja531:87.4_0": "dehi nesaṁ yadicchasi”.",
  "ja531:88.1_0": "Ekamekassa ekekaṁ,",
  "ja531:88.2_0": "adā sīhassaro kuso;",
  "ja531:88.3_0": "Khattiyānaṁ tadā tesaṁ,",
  "ja531:88.4_0": "rañño maddassa dhītaro.",
  "ja531:89.1_0": "Pīṇitā tena lābhena,",
  "ja531:89.2_0": "tuṭṭhā sīhassare kuse;",
  "ja531:89.3_0": "Sakaraṭṭhāni pāyiṁsu,",
  "ja531:89.4_0": "khattiyā satta tāvade.",
  "ja531:90.1_0": "Pabhāvatiñca ādāya,",
  "ja531:90.2_0": "maṇiṁ verocanaṁ subhaṁ;",
  "ja531:90.3_0": "Kusāvatiṁ kuso rājā,",
  "ja531:90.4_0": "agamāsi mahabbalo.",
  "ja531:91.1_0": "Tyassu ekarathe yantā,",
  "ja531:91.2_0": "pavisantā kusāvatiṁ;",
  "ja531:91.3_0": "Samānā vaṇṇarūpena,",
  "ja531:91.4_0": "nāññamaññātirocisuṁ.",
  "ja531:92.1_0": "Mātā puttena saṅgacchi,",
  "ja531:92.2_0": "ubhayo ca jayampatī;",
  "ja531:92.3_0": "Samaggā te tadā āsuṁ,",
  "ja531:92.4_0": "phītaṁ dharaṇimāvasunti.",
  "ja531:93.1_0": "Kusajātakaṁ paṭhamaṁ."
}