{
  "ja359:0.1_0": "Jātaka",
  "ja359:0.2_0": "Pañcakanipāta",
  "ja359:0.3_0": "Maṇikuṇḍalavagga",
  "ja359:0.4_0": "9. Suvaṇṇamigajātaka",
  "ja359:1.1_0": "“Vikkama re haripāda,",
  "ja359:1.2_0": "vikkama re mahāmiga;",
  "ja359:1.3_0": "Chinda vārattikaṁ pāsaṁ,",
  "ja359:1.4_0": "nāhaṁ ekā vane rame”.",
  "ja359:2.1_0": "“Vikkamāmi na pāremi,",
  "ja359:2.2_0": "bhūmiṁ sumbhāmi vegasā;",
  "ja359:2.3_0": "Daḷho vārattiko pāso,",
  "ja359:2.4_0": "pādaṁ me parikantati”.",
  "ja359:3.1_0": "“Attharassu palāsāni,",
  "ja359:3.2_0": "asiṁ nibbāha luddaka;",
  "ja359:3.3_0": "Paṭhamaṁ maṁ vadhitvāna,",
  "ja359:3.4_0": "hana pacchā mahāmigaṁ”.",
  "ja359:4.1_0": "“Na me sutaṁ vā diṭṭhaṁ vā,",
  "ja359:4.2_0": "Bhāsantiṁ mānusiṁ migiṁ;",
  "ja359:4.3_0": "Tvañca bhadde sukhī hohi,",
  "ja359:4.4_0": "Eso cāpi mahāmigo”.",
  "ja359:5.1_0": "“Evaṁ luddaka nandassu,",
  "ja359:5.2_0": "saha sabbehi ñātibhi;",
  "ja359:5.3_0": "Yathāhamajja nandāmi,",
  "ja359:5.4_0": "muttaṁ disvā mahāmigan”ti.",
  "ja359:6.1_0": "Suvaṇṇamigajātakaṁ navamaṁ."
}