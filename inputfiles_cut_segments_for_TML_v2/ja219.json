{
  "ja219:0.1_0": "Jātaka",
  "ja219:0.2_0": "Dukanipāta",
  "ja219:0.3_0": "Bīraṇathambhavagga",
  "ja219:0.4_0": "9. Garahitajātaka",
  "ja219:1.1_0": "“Hiraññaṁ me suvaṇṇaṁ me,",
  "ja219:1.2_0": "esā rattiṁ divā kathā;",
  "ja219:1.3_0": "Dummedhānaṁ manussānaṁ,",
  "ja219:1.4_0": "ariyadhammaṁ apassataṁ.",
  "ja219:2.1_0": "Dve dve gahapatayo gehe,",
  "ja219:2.2_0": "eko tattha amassuko;",
  "ja219:2.3_0": "Lambatthano veṇikato,",
  "ja219:2.4_0": "atho aṅkitakaṇṇako;",
  "ja219:2.5_0": "Kīto dhanena bahunā,",
  "ja219:2.6_0": "so taṁ vitudate janan”ti.",
  "ja219:3.1_0": "Garahitajātakaṁ navamaṁ."
}