{
  "ja244:0.1_0": "Jātaka",
  "ja244:0.2_0": "Dukanipāta",
  "ja244:0.3_0": "Siṅgālavagga",
  "ja244:0.4_0": "4. Vigaticchajātaka",
  "ja244:1.1_0": "“Yaṁ passati na taṁ icchati,",
  "ja244:1.2_0": "Yañca na passati taṁ kiricchati;",
  "ja244:1.3_0": "Maññāmi ciraṁ carissati,",
  "ja244:1.4_0": "Na hi taṁ lacchati yaṁ sa icchati.",
  "ja244:2.1_0": "Yaṁ labhati na tena tussati,",
  "ja244:2.2_0": "Yañca pattheti laddhaṁ hīḷeti;",
  "ja244:2.3_0": "Icchā hi anantagocarā,",
  "ja244:2.4_0": "Vigaticchāna namo karomase”ti.",
  "ja244:3.1_0": "Vigaticchajātakaṁ catutthaṁ."
}