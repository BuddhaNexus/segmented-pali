{
  "ja349:0.1_0": "Jātaka",
  "ja349:0.2_0": "Catukkanipāta",
  "ja349:0.3_0": "Cūḷakuṇālavagga",
  "ja349:0.4_0": "9. Sandhibhedajātaka",
  "ja349:1.1_0": "“Neva itthīsu sāmaññaṁ,",
  "ja349:1.2_0": "nāpi bhakkhesu sārathi;",
  "ja349:1.3_0": "Athassa sandhibhedassa,",
  "ja349:1.4_0": "passa yāva sucintitaṁ.",
  "ja349:2.1_0": "Asi tikkhova maṁsamhi,",
  "ja349:2.2_0": "pesuññaṁ parivattati;",
  "ja349:2.3_0": "Yatthūsabhañca sīhañca,",
  "ja349:2.4_0": "bhakkhayanti migādhamā.",
  "ja349:3.1_0": "Imaṁ so sayanaṁ seti,",
  "ja349:3.2_0": "yamimaṁ passasi sārathi;",
  "ja349:3.3_0": "Yo vācaṁ sandhibhedassa,",
  "ja349:3.4_0": "pisuṇassa nibodhati.",
  "ja349:4.1_0": "Te janā sukhamedhanti,",
  "ja349:4.2_0": "narā saggagatāriva;",
  "ja349:4.3_0": "Ye vācaṁ sandhibhedassa,",
  "ja349:4.4_0": "nāvabodhanti sārathī”ti.",
  "ja349:5.1_0": "Sandhibhedajātakaṁ navamaṁ."
}