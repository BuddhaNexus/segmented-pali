{
  "ne23:0.1_0": "Netti",
  "ne23:0.2_0": "Paṭiniddesavāra",
  "ne23:0.3_0": "Sampāta 4",
  "ne23:0.4_0": "Padaṭṭhānahārasampāta",
  "ne23:1.1_0": "Tattha katamo padaṭṭhāno hārasampāto?",
  "ne23:2.1_0": "“Tasmā rakkhitacittassa, sammāsaṅkappagocaro”ti gāthā.",
  "ne23:2.2_0": "“Tasmā rakkhitacittassā”ti tiṇṇaṁ sucaritānaṁ padaṭṭhānaṁ.",
  "ne23:2.3_0": "“Sammāsaṅkappagocaro”ti samathassa padaṭṭhānaṁ.",
  "ne23:2.4_0": "“Sammādiṭṭhipurekkhāro”ti vipassanāya padaṭṭhānaṁ.",
  "ne23:2.5_0": "“Ñatvāna udayabbayan”ti dassanabhūmiyā padaṭṭhānaṁ.",
  "ne23:2.6_0": "“Thinamiddhābhibhū bhikkhū”ti vīriyassa padaṭṭhānaṁ.",
  "ne23:2.7_0": "“Sabbā duggatiyo jahe”ti bhāvanāya padaṭṭhānaṁ.",
  "ne23:2.8_0": "Niyutto padaṭṭhāno hārasampāto."
}