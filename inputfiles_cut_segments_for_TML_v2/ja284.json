{
  "ja284:0.1_0": "Jātaka",
  "ja284:0.2_0": "Tikanipāta",
  "ja284:0.3_0": "Abbhantaravagga",
  "ja284:0.4_0": "4. Sirijātaka",
  "ja284:1.1_0": "“Yaṁ ussukā saṅgharanti,",
  "ja284:1.2_0": "alakkhikā bahuṁ dhanaṁ;",
  "ja284:1.3_0": "Sippavanto asippā ca,",
  "ja284:1.4_0": "lakkhivā tāni bhuñjati.",
  "ja284:2.1_0": "Sabbattha katapuññassa,",
  "ja284:2.2_0": "aticcaññeva pāṇino;",
  "ja284:2.3_0": "Uppajjanti bahū bhogā,",
  "ja284:2.4_0": "appanāyatanesupi.",
  "ja284:3.1_0": "Kukkuṭo maṇayo daṇḍo,",
  "ja284:3.2_0": "thiyo ca puññalakkhaṇā;",
  "ja284:3.3_0": "Uppajjanti apāpassa,",
  "ja284:3.4_0": "katapuññassa jantuno”ti.",
  "ja284:4.1_0": "Sirijātakaṁ catutthaṁ."
}