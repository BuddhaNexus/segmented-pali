{
  "tha-ap532:0.1_0": "Therāpadāna",
  "tha-ap532:0.2_0": "Tiṇadāyakavagga",
  "tha-ap532:0.3_0": "10. Sugandhattheraapadāna",
  "tha-ap532:1.1_0": "“Imamhi bhaddake kappe,",
  "tha-ap532:1.2_0": "brahmabandhu mahāyaso;",
  "tha-ap532:1.3_0": "Kassapo nāma gottena,",
  "tha-ap532:1.4_0": "uppajji vadataṁ varo.",
  "tha-ap532:2.1_0": "Anubyañjanasampanno,",
  "tha-ap532:2.2_0": "bāttiṁsavaralakkhaṇo;",
  "tha-ap532:2.3_0": "Byāmappabhāparivuto,",
  "tha-ap532:2.4_0": "raṁsijālasamotthaṭo.",
  "tha-ap532:3.1_0": "Assāsetā yathā cando,",
  "tha-ap532:3.2_0": "sūriyova pabhaṅkaro;",
  "tha-ap532:3.3_0": "Nibbāpetā yathā megho,",
  "tha-ap532:3.4_0": "sāgarova guṇākaro.",
  "tha-ap532:4.1_0": "Dharaṇīriva sīlena,",
  "tha-ap532:4.2_0": "himavāva samādhinā;",
  "tha-ap532:4.3_0": "Ākāso viya paññāya,",
  "tha-ap532:4.4_0": "asaṅgo anilo yathā.",
  "tha-ap532:5.1_0": "Sa kadāci mahāvīro,",
  "tha-ap532:5.2_0": "parisāsu visārado;",
  "tha-ap532:5.3_0": "Saccāni sampakāseti,",
  "tha-ap532:5.4_0": "uddharanto mahājanaṁ.",
  "tha-ap532:6.1_0": "Tadā hi bārāṇasiyaṁ,",
  "tha-ap532:6.2_0": "seṭṭhiputto mahāyaso;",
  "tha-ap532:6.3_0": "Āsahaṁ dhanadhaññassa,",
  "tha-ap532:6.4_0": "pahūtassa bahū tadā.",
  "tha-ap532:7.1_0": "Jaṅghāvihāraṁ vicaraṁ,",
  "tha-ap532:7.2_0": "migadāyamupeccahaṁ;",
  "tha-ap532:7.3_0": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap532:7.4_0": "desentaṁ amataṁ padaṁ.",
  "tha-ap532:8.1_0": "Visaṭṭhakantavacanaṁ,",
  "tha-ap532:8.2_0": "karavīkasamassaraṁ;",
  "tha-ap532:8.3_0": "Haṁsarutehi nigghosaṁ,",
  "tha-ap532:8.4_0": "viññāpentaṁ mahājanaṁ.",
  "tha-ap532:9.1_0": "Disvā devātidevaṁ taṁ,",
  "tha-ap532:9.2_0": "sutvāva madhuraṁ giraṁ;",
  "tha-ap532:9.3_0": "Pahāyanappake bhoge,",
  "tha-ap532:9.4_0": "pabbajiṁ anagāriyaṁ.",
  "tha-ap532:10.1_0": "Evaṁ pabbajito cāhaṁ,",
  "tha-ap532:10.2_0": "na cirena bahussuto;",
  "tha-ap532:10.3_0": "Ahosiṁ dhammakathiko,",
  "tha-ap532:10.4_0": "vicittapaṭibhānavā.",
  "tha-ap532:11.1_0": "Mahāparisamajjhehaṁ,",
  "tha-ap532:11.2_0": "haṭṭhacitto punappunaṁ;",
  "tha-ap532:11.3_0": "Vaṇṇayiṁ hemavaṇṇassa,",
  "tha-ap532:11.4_0": "vaṇṇaṁ vaṇṇavisārado.",
  "tha-ap532:12.1_0": "‘Esa khīṇāsavo buddho,",
  "tha-ap532:12.2_0": "anīgho chinnasaṁsayo;",
  "tha-ap532:12.3_0": "Sabbakammakkhayaṁ patto,",
  "tha-ap532:12.4_0": "vimuttopadhisaṅkhaye.",
  "tha-ap532:13.1_0": "Esa so bhagavā buddho,",
  "tha-ap532:13.2_0": "esa sīho anuttaro;",
  "tha-ap532:13.3_0": "Sadevakassa lokassa,",
  "tha-ap532:13.4_0": "brahmacakkappavattako.",
  "tha-ap532:14.1_0": "Danto dametā santo ca,",
  "tha-ap532:14.2_0": "sametā nibbuto isi;",
  "tha-ap532:14.3_0": "Nibbāpetā ca assattho,",
  "tha-ap532:14.4_0": "assāsetā mahājanaṁ.",
  "tha-ap532:15.1_0": "Vīro sūro ca vikkanto,",
  "tha-ap532:15.2_0": "pañño kāruṇiko vasī;",
  "tha-ap532:15.3_0": "Vijitāvī ca sa jino,",
  "tha-ap532:15.4_0": "appagabbo anālayo.",
  "tha-ap532:16.1_0": "Aneñjo acalo dhīmā,",
  "tha-ap532:16.2_0": "amoho asamo muni;",
  "tha-ap532:16.3_0": "Dhorayho usabho nāgo,",
  "tha-ap532:16.4_0": "sīho sakko garūsupi.",
  "tha-ap532:17.1_0": "Virāgo vimalo brahmā,",
  "tha-ap532:17.2_0": "vādī sūro raṇañjaho;",
  "tha-ap532:17.3_0": "Akhilo ca visallo ca,",
  "tha-ap532:17.4_0": "asamo saṁyato suci.",
  "tha-ap532:18.1_0": "Brāhmaṇo samaṇo nātho,",
  "tha-ap532:18.2_0": "bhisakko sallakattako;",
  "tha-ap532:18.3_0": "Yodho buddho sutāsuto,",
  "tha-ap532:18.4_0": "acalo mudito sito.",
  "tha-ap532:19.1_0": "Dhātā dhatā ca santi ca,",
  "tha-ap532:19.2_0": "kattā netā pakāsitā;",
  "tha-ap532:19.3_0": "Sampahaṁsitā bhettā ca,",
  "tha-ap532:19.4_0": "chettā sotā pasaṁsitā.",
  "tha-ap532:20.1_0": "Akhilo ca visallo ca,",
  "tha-ap532:20.2_0": "anīgho akathaṅkathī;",
  "tha-ap532:20.3_0": "Anejo virajo kattā,",
  "tha-ap532:20.4_0": "gandhā vattā pasaṁsitā.",
  "tha-ap532:21.1_0": "Tāretā atthakāretā,",
  "tha-ap532:21.2_0": "kāretā sampadāritā;",
  "tha-ap532:21.3_0": "Pāpetā sahitā kantā,",
  "tha-ap532:21.4_0": "hantā ātāpī tāpaso.",
  "tha-ap532:22.1_0": "Samacitto samasamo,",
  "tha-ap532:22.2_0": "asahāyo dayālayo;",
  "tha-ap532:22.3_0": "Accherasatto akuho,",
  "tha-ap532:22.4_0": "katāvī isisattamo.",
  "tha-ap532:23.1_0": "Nittiṇṇakaṅkho nimmāno,",
  "tha-ap532:23.2_0": "appameyyo anūpamo;",
  "tha-ap532:23.3_0": "Sabbavākyapathātīto,",
  "tha-ap532:23.4_0": "saccaneyyantagū jino.",
  "tha-ap532:24.1_0": "Sattasāravare tasmiṁ,",
  "tha-ap532:24.2_0": "Pasādo amatāvaho;",
  "tha-ap532:24.3_0": "Tasmā buddhe ca dhamme ca,",
  "tha-ap532:24.4_0": "Saṅghe saddhā mahatthikā’.",
  "tha-ap532:25.1_0": "Guṇehi evamādīhi,",
  "tha-ap532:25.2_0": "tilokasaraṇuttamaṁ;",
  "tha-ap532:25.3_0": "Vaṇṇento parisāmajjhe,",
  "tha-ap532:25.4_0": "akaṁ dhammakathaṁ ahaṁ.",
  "tha-ap532:26.1_0": "Tato cutāhaṁ tusite,",
  "tha-ap532:26.2_0": "anubhotvā mahāsukhaṁ;",
  "tha-ap532:26.3_0": "Tato cuto manussesu,",
  "tha-ap532:26.4_0": "jāto homi sugandhiko.",
  "tha-ap532:27.1_0": "Nissāso mukhagandho ca,",
  "tha-ap532:27.2_0": "dehagandho tatheva me;",
  "tha-ap532:27.3_0": "Sedagandho ca satataṁ,",
  "tha-ap532:27.4_0": "sabbagandhova hoti me.",
  "tha-ap532:28.1_0": "Mukhagandho sadā mayhaṁ,",
  "tha-ap532:28.2_0": "padumuppalacampako;",
  "tha-ap532:28.3_0": "Parisanto sadā vāti,",
  "tha-ap532:28.4_0": "sarīro ca tatheva me.",
  "tha-ap532:29.1_0": "Guṇatthavassa sabbantaṁ,",
  "tha-ap532:29.2_0": "Phalaṁ tu paramabbhutaṁ;",
  "tha-ap532:29.3_0": "Ekaggamanasā sabbe,",
  "tha-ap532:29.4_0": "Vaṇṇayissaṁ suṇātha me.",
  "tha-ap532:30.1_0": "Guṇaṁ buddhassa vatvāna,",
  "tha-ap532:30.2_0": "hitāya ca na sadisaṁ;",
  "tha-ap532:30.3_0": "Sukhito homi sabbattha,",
  "tha-ap532:30.4_0": "saṅgho vīrasamāyuto.",
  "tha-ap532:31.1_0": "Yasassī sukhito kanto,",
  "tha-ap532:31.2_0": "jutimā piyadassano;",
  "tha-ap532:31.3_0": "Vattā aparibhūto ca,",
  "tha-ap532:31.4_0": "niddoso paññavā tathā.",
  "tha-ap532:32.1_0": "Khīṇe āyusi nibbānaṁ,",
  "tha-ap532:32.2_0": "sulabhaṁ buddhabhattino;",
  "tha-ap532:32.3_0": "Tesaṁ hetuṁ pavakkhāmi,",
  "tha-ap532:32.4_0": "taṁ suṇātha yathātathaṁ.",
  "tha-ap532:33.1_0": "Santaṁ yasaṁ bhagavato,",
  "tha-ap532:33.2_0": "vidhinā abhivādayaṁ;",
  "tha-ap532:33.3_0": "Tattha tatthūpapannopi,",
  "tha-ap532:33.4_0": "yasassī tena homahaṁ.",
  "tha-ap532:34.1_0": "Dukkhassantakaraṁ buddhaṁ,",
  "tha-ap532:34.2_0": "dhammaṁ santamasaṅkhataṁ;",
  "tha-ap532:34.3_0": "Vaṇṇayaṁ sukhado āsiṁ,",
  "tha-ap532:34.4_0": "sattānaṁ sukhito tato.",
  "tha-ap532:35.1_0": "Guṇaṁ vadanto buddhassa,",
  "tha-ap532:35.2_0": "buddhapītisamāyuto;",
  "tha-ap532:35.3_0": "Sakantiṁ parakantiñca,",
  "tha-ap532:35.4_0": "janayiṁ tena kantimā.",
  "tha-ap532:36.1_0": "Jino te titthikākiṇṇe,",
  "tha-ap532:36.2_0": "abhibhuyya kutitthiye;",
  "tha-ap532:36.3_0": "Guṇaṁ vadanto jotesiṁ,",
  "tha-ap532:36.4_0": "nāyakaṁ jutimā tato.",
  "tha-ap532:37.1_0": "Piyakārī janassāpi,",
  "tha-ap532:37.2_0": "sambuddhassa guṇaṁ vadaṁ;",
  "tha-ap532:37.3_0": "Saradova sasaṅkohaṁ,",
  "tha-ap532:37.4_0": "tenāsiṁ piyadassano.",
  "tha-ap532:38.1_0": "Yathāsattivasenāhaṁ,",
  "tha-ap532:38.2_0": "sabbavācāhi santhaviṁ;",
  "tha-ap532:38.3_0": "Sugataṁ tena vāgiso,",
  "tha-ap532:38.4_0": "vicittapaṭibhānavā.",
  "tha-ap532:39.1_0": "Ye bālā vimatiṁ pattā,",
  "tha-ap532:39.2_0": "paribhonti mahāmuniṁ;",
  "tha-ap532:39.3_0": "Niggahiṁ te saddhammena,",
  "tha-ap532:39.4_0": "paribhūto na tenahaṁ.",
  "tha-ap532:40.1_0": "Buddhavaṇṇena sattānaṁ,",
  "tha-ap532:40.2_0": "kilese apanesahaṁ;",
  "tha-ap532:40.3_0": "Nikkilesamano homi,",
  "tha-ap532:40.4_0": "tassa kammassa vāhasā.",
  "tha-ap532:41.1_0": "Sotūnaṁ vuddhimajaniṁ,",
  "tha-ap532:41.2_0": "buddhānussatidesako;",
  "tha-ap532:41.3_0": "Tenāhamāsiṁ sappañño,",
  "tha-ap532:41.4_0": "nipuṇatthavipassako.",
  "tha-ap532:42.1_0": "Sabbāsavaparikkhīṇo,",
  "tha-ap532:42.2_0": "tiṇṇasaṁsārasāgaro;",
  "tha-ap532:42.3_0": "Sikhīva anupādāno,",
  "tha-ap532:42.4_0": "pāpuṇissāmi nibbutiṁ.",
  "tha-ap532:43.1_0": "Imasmiṁyeva kappasmiṁ,",
  "tha-ap532:43.2_0": "yamahaṁ santhaviṁ jinaṁ;",
  "tha-ap532:43.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap532:43.4_0": "buddhavaṇṇassidaṁ phalaṁ.",
  "tha-ap532:44.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap532:44.2_0": "…pe…",
  "tha-ap532:44.3_0": "viharāmi anāsavo.",
  "tha-ap532:45.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap532:45.2_0": "…pe…",
  "tha-ap532:45.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap532:46.1_0": "Paṭisambhidā catasso,",
  "tha-ap532:46.2_0": "…pe…",
  "tha-ap532:46.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap532:47.1_0": "Itthaṁ sudaṁ āyasmā sugandho thero imā gāthāyo abhāsitthāti.",
  "tha-ap532:48.1_0": "Sugandhattherassāpadānaṁ dasamaṁ.",
  "tha-ap532:49.1_0": "Tiṇadāyakavaggo tepaññāsamo.",
  "tha-ap532:50.1_0": "Tassuddānaṁ",
  "tha-ap532:51.1_0": "Tiṇado mañcado ceva,",
  "tha-ap532:51.2_0": "saraṇabbhañjanappado;",
  "tha-ap532:51.3_0": "Supaṭo daṇḍadāyī ca,",
  "tha-ap532:51.4_0": "nelapūjī tatheva ca.",
  "tha-ap532:52.1_0": "Bodhisammajjako maṇḍo,",
  "tha-ap532:52.2_0": "sugandho dasamoti ca;",
  "tha-ap532:52.3_0": "Gāthāsataṁ satevīsaṁ,",
  "tha-ap532:52.4_0": "gaṇitañcettha sabbaso."
}