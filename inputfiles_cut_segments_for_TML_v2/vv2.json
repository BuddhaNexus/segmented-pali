{
  "vv2:0.1_0": "Vimānavatthu",
  "vv2:0.2_0": "Itthivimāna",
  "vv2:0.3_0": "Pīṭhavagga",
  "vv2:0.4_0": "2. Dutiyapīṭhavimānavatthu",
  "vv2:1.1_0": "“Pīṭhaṁ te veḷuriyamayaṁ uḷāraṁ,",
  "vv2:1.2_0": "Manojavaṁ gacchati yenakāmaṁ;",
  "vv2:1.3_0": "Alaṅkate malyadhare suvatthe,",
  "vv2:1.4_0": "Obhāsasi vijjurivabbhakūṭaṁ.",
  "vv2:2.1_0": "Kena tetādiso vaṇṇo,",
  "vv2:2.2_0": "kena te idha mijjhati;",
  "vv2:2.3_0": "Uppajjanti ca te bhogā,",
  "vv2:2.4_0": "ye keci manaso piyā.",
  "vv2:3.1_0": "Pucchāmi taṁ devi mahānubhāve,",
  "vv2:3.2_0": "Manussabhūtā kimakāsi puññaṁ;",
  "vv2:3.3_0": "Kenāsi evaṁ jalitānubhāvā,",
  "vv2:3.4_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv2:4.1_0": "Sā devatā attamanā,",
  "vv2:4.2_0": "moggallānena pucchitā;",
  "vv2:4.3_0": "Pañhaṁ puṭṭhā viyākāsi,",
  "vv2:4.4_0": "yassa kammassidaṁ phalaṁ.",
  "vv2:5.1_0": "“Ahaṁ manussesu manussabhūtā,",
  "vv2:5.2_0": "Abbhāgatānāsanakaṁ adāsiṁ;",
  "vv2:5.3_0": "Abhivādayiṁ añjalikaṁ akāsiṁ,",
  "vv2:5.4_0": "Yathānubhāvañca adāsi dānaṁ.",
  "vv2:6.1_0": "Tena metādiso vaṇṇo,",
  "vv2:6.2_0": "tena me idha mijjhati;",
  "vv2:6.3_0": "Uppajjanti ca me bhogā,",
  "vv2:6.4_0": "ye keci manaso piyā.",
  "vv2:7.1_0": "Akkhāmi te bhikkhu mahānubhāva,",
  "vv2:7.2_0": "Manussabhūtā yamakāsi puññaṁ;",
  "vv2:7.3_0": "Tenamhi evaṁ jalitānubhāvā,",
  "vv2:7.4_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv2:8.1_0": "Dutiyapīṭhavimānaṁ dutiyaṁ."
}