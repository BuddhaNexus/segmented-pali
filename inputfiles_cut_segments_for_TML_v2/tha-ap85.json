{
  "tha-ap85:0.1_0": "Therāpadāna",
  "tha-ap85:0.2_0": "Timiravagga",
  "tha-ap85:0.3_0": "3 Nipannañjalikattheraapadāna",
  "tha-ap85:1.1_0": "“Rukkhamūle nisinnohaṁ,",
  "tha-ap85:1.2_0": "byādhito paramena ca;",
  "tha-ap85:1.3_0": "Paramakāruññapattomhi,",
  "tha-ap85:1.4_0": "araññe kānane ahaṁ.",
  "tha-ap85:2.1_0": "Anukampaṁ upādāya,",
  "tha-ap85:2.2_0": "tisso satthā upesi maṁ;",
  "tha-ap85:2.3_0": "Sohaṁ nipannako santo,",
  "tha-ap85:2.4_0": "sire katvāna añjaliṁ.",
  "tha-ap85:3.1_0": "Pasannacitto sumano,",
  "tha-ap85:3.2_0": "sabbasattānamuttamaṁ;",
  "tha-ap85:3.3_0": "Sambuddhaṁ abhivādetvā,",
  "tha-ap85:3.4_0": "tattha kālaṅkato ahaṁ.",
  "tha-ap85:4.1_0": "Dvenavute ito kappe,",
  "tha-ap85:4.2_0": "yaṁ vandiṁ purisuttamaṁ;",
  "tha-ap85:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap85:4.4_0": "vandanāya idaṁ phalaṁ.",
  "tha-ap85:5.1_0": "Ito pañcamake kappe,",
  "tha-ap85:5.2_0": "pañcevāsuṁ mahāsikhā;",
  "tha-ap85:5.3_0": "Sattaratanasampannā,",
  "tha-ap85:5.4_0": "cakkavattī mahabbalā.",
  "tha-ap85:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap85:6.2_0": "…pe…",
  "tha-ap85:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap85:7.1_0": "Itthaṁ sudaṁ āyasmā nipannañjaliko thero imā gāthāyo abhāsitthāti.",
  "tha-ap85:8.1_0": "Nipannañjalikattherassāpadānaṁ tatiyaṁ."
}