{
  "thig13.1:0.1_0": "Therīgāthā",
  "thig13.1:0.2_0": "Vīsatinipāta",
  "thig13.1:0.3_0": "1. Ambapālītherīgāthā",
  "thig13.1:1.1_0": "“Kāḷakā bhamaravaṇṇasādisā,",
  "thig13.1:1.2_0": "Vellitaggā mama muddhajā ahuṁ;",
  "thig13.1:1.3_0": "Te jarāya sāṇavākasādisā,",
  "thig13.1:1.4_0": "Saccavādivacanaṁ anaññathā.",
  "thig13.1:2.1_0": "Vāsitova surabhī karaṇḍako,",
  "thig13.1:2.2_0": "Pupphapūra mama uttamaṅgajo;",
  "thig13.1:2.3_0": "Taṁ jarāyatha salomagandhikaṁ,",
  "thig13.1:2.4_0": "Saccavādivacanaṁ anaññathā.",
  "thig13.1:3.1_0": "Kānanaṁva sahitaṁ suropitaṁ,",
  "thig13.1:3.2_0": "Kocchasūcivicitaggasobhitaṁ;",
  "thig13.1:3.3_0": "Taṁ jarāya viralaṁ tahiṁ tahiṁ,",
  "thig13.1:3.4_0": "Saccavādivacanaṁ anaññathā.",
  "thig13.1:4.1_0": "Kaṇhakhandhakasuvaṇṇamaṇḍitaṁ,",
  "thig13.1:4.2_0": "Sobhate suveṇīhilaṅkataṁ;",
  "thig13.1:4.3_0": "Taṁ jarāya khalitaṁ siraṁ kataṁ,",
  "thig13.1:4.4_0": "Saccavādivacanaṁ anaññathā.",
  "thig13.1:5.1_0": "Cittakārasukatāva lekhikā,",
  "thig13.1:5.2_0": "Sobhare su bhamukā pure mama;",
  "thig13.1:5.3_0": "Tā jarāya valibhippalambitā,",
  "thig13.1:5.4_0": "Saccavādivacanaṁ anaññathā.",
  "thig13.1:6.1_0": "Bhassarā surucirā yathā maṇī,",
  "thig13.1:6.2_0": "Nettahesumabhinīlamāyatā;",
  "thig13.1:6.3_0": "Te jarāyabhihatā na sobhare,",
  "thig13.1:6.4_0": "Saccavādivacanaṁ anaññathā.",
  "thig13.1:7.1_0": "Saṇhatuṅgasadisī ca nāsikā,",
  "thig13.1:7.2_0": "Sobhate su abhiyobbanaṁ pati;",
  "thig13.1:7.3_0": "Sā jarāya upakūlitā viya,",
  "thig13.1:7.4_0": "Saccavādivacanaṁ anaññathā.",
  "thig13.1:8.1_0": "Kaṅkaṇaṁva sukataṁ suniṭṭhitaṁ,",
  "thig13.1:8.2_0": "Sobhare su mama kaṇṇapāḷiyo;",
  "thig13.1:8.3_0": "Tā jarāya valibhippalambitā,",
  "thig13.1:8.4_0": "Saccavādivacanaṁ anaññathā.",
  "thig13.1:9.1_0": "Pattalīmakulavaṇṇasādisā,",
  "thig13.1:9.2_0": "Sobhare su dantā pure mama;",
  "thig13.1:9.3_0": "Te jarāya khaṇḍitā cāsitā,",
  "thig13.1:9.4_0": "Saccavādivacanaṁ anaññathā.",
  "thig13.1:10.1_0": "Kānanamhi vanasaṇḍacārinī,",
  "thig13.1:10.2_0": "Kokilāva madhuraṁ nikūjihaṁ;",
  "thig13.1:10.3_0": "Taṁ jarāya khalitaṁ tahiṁ tahiṁ,",
  "thig13.1:10.4_0": "Saccavādivacanaṁ anaññathā.",
  "thig13.1:11.1_0": "Saṇhakamburiva suppamajjitā,",
  "thig13.1:11.2_0": "Sobhate su gīvā pure mama;",
  "thig13.1:11.3_0": "Sā jarāya bhaggā vināmitā,",
  "thig13.1:11.4_0": "Saccavādivacanaṁ anaññathā.",
  "thig13.1:12.1_0": "Vaṭṭapalighasadisopamā ubho,",
  "thig13.1:12.2_0": "Sobhare su bāhā pure mama;",
  "thig13.1:12.3_0": "Tā jarāya yatha pāṭalibbalitā,",
  "thig13.1:12.4_0": "Saccavādivacanaṁ anaññathā.",
  "thig13.1:13.1_0": "Saṇhamuddikasuvaṇṇamaṇḍitā,",
  "thig13.1:13.2_0": "Sobhare su hatthā pure mama;",
  "thig13.1:13.3_0": "Te jarāya yathā mūlamūlikā,",
  "thig13.1:13.4_0": "Saccavādivacanaṁ anaññathā.",
  "thig13.1:14.1_0": "Pīnavaṭṭasahituggatā ubho,",
  "thig13.1:14.2_0": "Sobhare su thanakā pure mama;",
  "thig13.1:14.3_0": "Thevikīva lambanti nodakā,",
  "thig13.1:14.4_0": "Saccavādivacanaṁ anaññathā.",
  "thig13.1:15.1_0": "Kañcanassa phalakaṁva sammaṭṭhaṁ,",
  "thig13.1:15.2_0": "Sobhate su kāyo pure mama;",
  "thig13.1:15.3_0": "So valīhi sukhumāhi otato,",
  "thig13.1:15.4_0": "Saccavādivacanaṁ anaññathā.",
  "thig13.1:16.1_0": "Nāgabhogasadisopamā ubho,",
  "thig13.1:16.2_0": "Sobhare su ūrū pure mama;",
  "thig13.1:16.3_0": "Te jarāya yathā veḷunāḷiyo,",
  "thig13.1:16.4_0": "Saccavādivacanaṁ anaññathā.",
  "thig13.1:17.1_0": "Saṇhanūpurasuvaṇṇamaṇḍitā,",
  "thig13.1:17.2_0": "Sobhare su jaṅghā pure mama;",
  "thig13.1:17.3_0": "Tā jarāya tiladaṇḍakāriva,",
  "thig13.1:17.4_0": "Saccavādivacanaṁ anaññathā.",
  "thig13.1:18.1_0": "Tūlapuṇṇasadisopamā ubho,",
  "thig13.1:18.2_0": "Sobhare su pādā pure mama;",
  "thig13.1:18.3_0": "Te jarāya phuṭitā valīmatā,",
  "thig13.1:18.4_0": "Saccavādivacanaṁ anaññathā.",
  "thig13.1:19.1_0": "Ediso ahu ayaṁ samussayo,",
  "thig13.1:19.2_0": "Jajjaro bahudukkhānamālayo;",
  "thig13.1:19.3_0": "Sopalepapatito jarāgharo,",
  "thig13.1:19.4_0": "Saccavādivacanaṁ anaññathā”.",
  "thig13.1:20.1_0": "… Ambapālī therī …."
}