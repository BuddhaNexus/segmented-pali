{
  "thig6.4:0.1_0": "Therīgāthā",
  "thig6.4:0.2_0": "Chakkanipāta",
  "thig6.4:0.3_0": "4. Sujātātherīgāthā",
  "thig6.4:1.1_0": "“Alaṅkatā suvasanā,",
  "thig6.4:1.2_0": "mālinī candanokkhitā;",
  "thig6.4:1.3_0": "Sabbābharaṇasañchannā,",
  "thig6.4:1.4_0": "dāsīgaṇapurakkhatā.",
  "thig6.4:2.1_0": "Annaṁ pānañca ādāya,",
  "thig6.4:2.2_0": "khajjaṁ bhojjaṁ anappakaṁ;",
  "thig6.4:2.3_0": "Gehato nikkhamitvāna,",
  "thig6.4:2.4_0": "uyyānamabhihārayiṁ.",
  "thig6.4:3.1_0": "Tattha ramitvā kīḷitvā,",
  "thig6.4:3.2_0": "āgacchantī sakaṁ gharaṁ;",
  "thig6.4:3.3_0": "Vihāraṁ daṭṭhuṁ pāvisiṁ,",
  "thig6.4:3.4_0": "sākete añjanaṁ vanaṁ.",
  "thig6.4:4.1_0": "Disvāna lokapajjotaṁ,",
  "thig6.4:4.2_0": "vanditvāna upāvisiṁ;",
  "thig6.4:4.3_0": "So me dhammamadesesi,",
  "thig6.4:4.4_0": "anukampāya cakkhumā.",
  "thig6.4:5.1_0": "Sutvā ca kho mahesissa,",
  "thig6.4:5.2_0": "saccaṁ sampaṭivijjhahaṁ;",
  "thig6.4:5.3_0": "Tattheva virajaṁ dhammaṁ,",
  "thig6.4:5.4_0": "phusayiṁ amataṁ padaṁ.",
  "thig6.4:6.1_0": "Tato viññātasaddhammā,",
  "thig6.4:6.2_0": "pabbajiṁ anagāriyaṁ;",
  "thig6.4:6.3_0": "Tisso vijjā anuppattā,",
  "thig6.4:6.4_0": "amoghaṁ buddhasāsanan”ti.",
  "thig6.4:7.1_0": "… Sujātā therī …."
}