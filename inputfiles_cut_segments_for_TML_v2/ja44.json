{
  "ja44:0.1_0": "Jātaka",
  "ja44:0.2_0": "Ekakanipāta",
  "ja44:0.3_0": "Atthakāmavagga",
  "ja44:0.4_0": "4. Makasajātaka",
  "ja44:1.1_0": "“Seyyo amitto matiyā upeto,",
  "ja44:1.2_0": "Na tveva mitto mativippahīno;",
  "ja44:1.3_0": "Makasaṁ vadhissanti hi eḷamūgo,",
  "ja44:1.4_0": "Putto pitu abbhidā uttamaṅgan”ti.",
  "ja44:2.1_0": "Makasajātakaṁ catutthaṁ."
}