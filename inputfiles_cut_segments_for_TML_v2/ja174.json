{
  "ja174:0.1_0": "Jātaka",
  "ja174:0.2_0": "Dukanipāta",
  "ja174:0.3_0": "Kalyāṇavagga",
  "ja174:0.4_0": "4. Dubbhiyamakkaṭajātaka",
  "ja174:1.1_0": "“Adamha te vāri pahūtarūpaṁ,",
  "ja174:1.2_0": "Ghammābhitattassa pipāsitassa;",
  "ja174:1.3_0": "So dāni pitvāna kiriṅkarosi,",
  "ja174:1.4_0": "Asaṅgamo pāpajanena seyyo”.",
  "ja174:2.1_0": "“Ko te suto vā diṭṭho vā,",
  "ja174:2.2_0": "sīlavā nāma makkaṭo;",
  "ja174:2.3_0": "Idāni kho taṁ ohacchaṁ,",
  "ja174:2.4_0": "esā asmāka dhammatā”ti.",
  "ja174:3.1_0": "Dubbhiyamakkaṭajātakaṁ catutthaṁ."
}