{
  "tha-ap220:0.1_0": "Therāpadāna",
  "tha-ap220:0.2_0": "Hatthivagga",
  "tha-ap220:0.3_0": "8. Akkantasaññakattheraapadāna",
  "tha-ap220:1.1_0": "“Kusāṭakaṁ gahetvāna,",
  "tha-ap220:1.2_0": "upajjhāyassahaṁ pure;",
  "tha-ap220:1.3_0": "Mantañca anusikkhāmi,",
  "tha-ap220:1.4_0": "ganthādosassa pattiyā.",
  "tha-ap220:2.1_0": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap220:2.2_0": "Āhutīnaṁ paṭiggahaṁ;",
  "tha-ap220:2.3_0": "Usabhaṁ pavaraṁ aggaṁ,",
  "tha-ap220:2.4_0": "Tissaṁ buddhaṁ gaṇuttamaṁ.",
  "tha-ap220:3.1_0": "Kusāṭakaṁ pattharitaṁ,",
  "tha-ap220:3.2_0": "akkamantaṁ naruttamaṁ;",
  "tha-ap220:3.3_0": "Samuggataṁ mahāvīraṁ,",
  "tha-ap220:3.4_0": "lokajeṭṭhaṁ narāsabhaṁ.",
  "tha-ap220:4.1_0": "Disvā taṁ lokapajjotaṁ,",
  "tha-ap220:4.2_0": "vimalaṁ candasannibhaṁ;",
  "tha-ap220:4.3_0": "Avandiṁ satthuno pāde,",
  "tha-ap220:4.4_0": "vippasannena cetasā.",
  "tha-ap220:5.1_0": "Catunnavutito kappe,",
  "tha-ap220:5.2_0": "yaṁ adāsiṁ kusāṭakaṁ;",
  "tha-ap220:5.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap220:5.4_0": "kusāṭakassidaṁ phalaṁ.",
  "tha-ap220:6.1_0": "Sattatiṁse ito kappe,",
  "tha-ap220:6.2_0": "eko āsiṁ janādhipo;",
  "tha-ap220:6.3_0": "Sunando nāma nāmena,",
  "tha-ap220:6.4_0": "cakkavattī mahabbalo.",
  "tha-ap220:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap220:7.2_0": "…pe…",
  "tha-ap220:7.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap220:8.1_0": "Itthaṁ sudaṁ āyasmā akkantasaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap220:9.1_0": "Akkantasaññakattherassāpadānaṁ aṭṭhamaṁ."
}