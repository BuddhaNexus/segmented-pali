{
  "pv50:0.1_0": "Petavatthu",
  "pv50:0.2_0": "Mahāvagga",
  "pv50:0.3_0": "15. Seṭṭhiputtapetavatthu",
  "pv50:1.1_0": "“Saṭṭhivassasahassāni,",
  "pv50:1.2_0": "paripuṇṇāni sabbaso;",
  "pv50:1.3_0": "Niraye paccamānānaṁ,",
  "pv50:1.4_0": "kadā anto bhavissati.",
  "pv50:2.1_0": "Natthi anto kuto anto,",
  "pv50:2.2_0": "na anto paṭidissati;",
  "pv50:2.3_0": "Tathā hi pakataṁ pāpaṁ,",
  "pv50:2.4_0": "tuyhaṁ mayhañca mārisā.",
  "pv50:3.1_0": "Dujjīvitamajīvamha,",
  "pv50:3.2_0": "ye sante na dadamhase;",
  "pv50:3.3_0": "Santesu deyyadhammesu,",
  "pv50:3.4_0": "dīpaṁ nākamha attano.",
  "pv50:4.1_0": "Sohaṁ nūna ito gantvā,",
  "pv50:4.2_0": "yoniṁ laddhāna mānusiṁ;",
  "pv50:4.3_0": "Vadaññū sīlasampanno,",
  "pv50:4.4_0": "kāhāmi kusalaṁ bahun”ti.",
  "pv50:5.1_0": "Seṭṭhiputtapetavatthu pannarasamaṁ."
}