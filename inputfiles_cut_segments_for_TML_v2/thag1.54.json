{
  "thag1.54:0.1_0": "Theragāthā",
  "thag1.54:0.2_0": "Ekakanipāta",
  "thag1.54:0.3_0": "Chaṭṭhavagga",
  "thag1.54:0.4_0": "1.54. Uttiyattheragāthā",
  "thag1.54:1.1_0": "“Vassati devo yathā sugītaṁ,",
  "thag1.54:1.2_0": "Channā me kuṭikā sukhā nivātā;",
  "thag1.54:1.3_0": "Tassaṁ viharāmi adutiyo,",
  "thag1.54:1.4_0": "Atha ce patthayasi pavassa devā”ti.",
  "thag1.54:1.5_0": "… Uttiyo thero …."
}