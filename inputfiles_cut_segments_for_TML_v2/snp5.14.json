{
  "snp5.14:0.1_0": "Sutta Nipāta 5.14",
  "snp5.14:0.2_0": "Udayamāṇavapucchā",
  "snp5.14:1.1_0": "“Jhāyiṁ virajamāsīnaṁ,",
  "snp5.14:1.2_0": "(iccāyasmā udayo)",
  "snp5.14:1.3_0": "Katakiccaṁ anāsavaṁ;",
  "snp5.14:1.4_0": "Pāraguṁ sabbadhammānaṁ,",
  "snp5.14:1.5_0": "Atthi pañhena āgamaṁ;",
  "snp5.14:1.6_0": "Aññāvimokkhaṁ pabrūhi,",
  "snp5.14:1.7_0": "Avijjāya pabhedanaṁ”.",
  "snp5.14:2.1_0": "“Pahānaṁ kāmacchandānaṁ,",
  "snp5.14:2.2_0": "(udayāti bhagavā)",
  "snp5.14:2.3_0": "Domanassāna cūbhayaṁ;",
  "snp5.14:2.4_0": "Thinassa ca panūdanaṁ,",
  "snp5.14:2.5_0": "Kukkuccānaṁ nivāraṇaṁ.",
  "snp5.14:3.1_0": "Upekkhāsatisaṁsuddhaṁ,",
  "snp5.14:3.2_0": "dhammatakkapurejavaṁ;",
  "snp5.14:3.3_0": "Aññāvimokkhaṁ pabrūmi,",
  "snp5.14:3.4_0": "avijjāya pabhedanaṁ”.",
  "snp5.14:4.1_0": "“Kiṁsu saṁyojano loko,",
  "snp5.14:4.2_0": "kiṁsu tassa vicāraṇaṁ;",
  "snp5.14:4.3_0": "Kissassa vippahānena,",
  "snp5.14:4.4_0": "nibbānaṁ iti vuccati”.",
  "snp5.14:5.1_0": "“Nandisaṁyojano loko,",
  "snp5.14:5.2_0": "vitakkassa vicāraṇaṁ;",
  "snp5.14:5.3_0": "Taṇhāya vippahānena,",
  "snp5.14:5.4_0": "nibbānaṁ iti vuccati”.",
  "snp5.14:6.1_0": "“Kathaṁ satassa carato,",
  "snp5.14:6.2_0": "viññāṇaṁ uparujjhati;",
  "snp5.14:6.3_0": "Bhagavantaṁ puṭṭhumāgamma,",
  "snp5.14:6.4_0": "taṁ suṇoma vaco tava”.",
  "snp5.14:7.1_0": "“Ajjhattañca bahiddhā ca,",
  "snp5.14:7.2_0": "Vedanaṁ nābhinandato;",
  "snp5.14:7.3_0": "Evaṁ satassa carato,",
  "snp5.14:7.4_0": "Viññāṇaṁ uparujjhatī”ti.",
  "snp5.14:7.5_0": "Udayamāṇavapucchā terasamā."
}