{
  "ja506:0.1_0": "Jātaka",
  "ja506:0.2_0": "Vīsatinipāta",
  "ja506:0.3_0": "Mātaṅgavagga",
  "ja506:0.4_0": "10. Campeyyajātaka",
  "ja506:1.1_0": "“Kā nu vijjurivābhāsi,",
  "ja506:1.2_0": "Osadhī viya tārakā;",
  "ja506:1.3_0": "Devatā nusi gandhabbī,",
  "ja506:1.4_0": "Na taṁ maññāmi mānusiṁ”.",
  "ja506:2.1_0": "“Namhi devī na gandhabbī,",
  "ja506:2.2_0": "na mahārāja mānusī;",
  "ja506:2.3_0": "Nāgakaññāsmi bhaddante,",
  "ja506:2.4_0": "atthenamhi idhāgatā”.",
  "ja506:3.1_0": "“Vibbhantacittā kupitindriyāsi,",
  "ja506:3.2_0": "Nettehi te vārigaṇā savanti;",
  "ja506:3.3_0": "Kiṁ te naṭṭhaṁ kiṁ pana patthayānā,",
  "ja506:3.4_0": "Idhāgatā nāri tadiṅgha brūhi”.",
  "ja506:4.1_0": "“Yamuggatejo uragoti cāhu,",
  "ja506:4.2_0": "Nāgoti naṁ āhu janā janinda;",
  "ja506:4.3_0": "Tamaggahī puriso jīvikattho,",
  "ja506:4.4_0": "Taṁ bandhanā muñca patī mameso”.",
  "ja506:5.1_0": "“Kathaṁ nvayaṁ balaviriyūpapanno,",
  "ja506:5.2_0": "Hatthattamāgacchi vanibbakassa;",
  "ja506:5.3_0": "Akkhāhi me nāgakaññe tamatthaṁ,",
  "ja506:5.4_0": "Kathaṁ vijānemu gahītanāgaṁ”.",
  "ja506:6.1_0": "“Nagarampi nāgo bhasmaṁ kareyya,",
  "ja506:6.2_0": "Tathā hi so balaviriyūpapanno;",
  "ja506:6.3_0": "Dhammañca nāgo apacāyamāno,",
  "ja506:6.4_0": "Tasmā parakkamma tapo karoti.",
  "ja506:7.1_0": "Cātuddasiṁ pañcadasiñca rāja,",
  "ja506:7.2_0": "Catuppathe sammati nāgarājā;",
  "ja506:7.3_0": "Tamaggahī puriso jīvikattho,",
  "ja506:7.4_0": "Taṁ bandhanā muñca patī mameso”.",
  "ja506:8.1_0": "“Soḷasitthisahassāni,",
  "ja506:8.2_0": "āmuttamaṇikuṇḍalā;",
  "ja506:8.3_0": "Vārigehasayā nārī,",
  "ja506:8.4_0": "tāpi taṁ saraṇaṁ gatā.",
  "ja506:9.1_0": "Dhammena mocehi asāhasena,",
  "ja506:9.2_0": "Gāmena nikkhena gavaṁ satena;",
  "ja506:9.3_0": "Ossaṭṭhakāyo urago carātu,",
  "ja506:9.4_0": "Puññatthiko muñcatu bandhanasmā”.",
  "ja506:10.1_0": "“Dhammena mocemi asāhasena,",
  "ja506:10.2_0": "Gāmena nikkhena gavaṁ satena;",
  "ja506:10.3_0": "Ossaṭṭhakāyo urago carātu,",
  "ja506:10.4_0": "Puññatthiko muñcatu bandhanasmā.",
  "ja506:11.1_0": "Dammi nikkhasataṁ ludda,",
  "ja506:11.2_0": "thūlañca maṇikuṇḍalaṁ;",
  "ja506:11.3_0": "Catussadañca pallaṅkaṁ,",
  "ja506:11.4_0": "umāpupphasarinnibhaṁ.",
  "ja506:12.1_0": "Dve ca sādisiyo bhariyā,",
  "ja506:12.2_0": "Usabhañca gavaṁ sataṁ;",
  "ja506:12.3_0": "Ossaṭṭhakāyo urago carātu,",
  "ja506:12.4_0": "Puññatthiko muñcatu bandhanasmā”.",
  "ja506:13.1_0": "“Vināpi dānā tava vacanaṁ janinda,",
  "ja506:13.2_0": "Muñcemu naṁ uragaṁ bandhanasmā;",
  "ja506:13.3_0": "Ossaṭṭhakāyo urago carātu,",
  "ja506:13.4_0": "Puññatthiko muñcatu bandhanasmā”.",
  "ja506:14.1_0": "Mutto campeyyako nāgo,",
  "ja506:14.2_0": "rājānaṁ etadabravi;",
  "ja506:14.3_0": "“Namo te kāsirājatthu,",
  "ja506:14.4_0": "namo te kāsivaḍḍhana;",
  "ja506:14.5_0": "Añjaliṁ te paggaṇhāmi,",
  "ja506:14.6_0": "passeyyaṁ me nivesanaṁ”.",
  "ja506:15.1_0": "“Addhā hi dubbissasametamāhu,",
  "ja506:15.2_0": "Yaṁ mānuso vissase amānusamhi;",
  "ja506:15.3_0": "Sace ca maṁ yācasi etamatthaṁ,",
  "ja506:15.4_0": "Dakkhemu te nāga nivesanāni”.",
  "ja506:16.1_0": "“Sacepi vāto girimāvaheyya,",
  "ja506:16.2_0": "Cando ca suriyo ca chamā pateyyuṁ;",
  "ja506:16.3_0": "Sabbā ca najjo paṭisotaṁ vajeyyuṁ,",
  "ja506:16.4_0": "Na tvevahaṁ rāja musā bhaṇeyyaṁ.",
  "ja506:17.1_0": "Nabhaṁ phaleyya udadhīpi susse,",
  "ja506:17.2_0": "Saṁvaṭṭaye bhūtadharā vasundharā;",
  "ja506:17.3_0": "Siluccayo meru samūlamuppate,",
  "ja506:17.4_0": "Na tvevahaṁ rāja musā bhaṇeyyaṁ”.",
  "ja506:18.1_0": "“Addhā hi dubbissasametamāhu,",
  "ja506:18.2_0": "Yaṁ mānuso vissase amānusamhi;",
  "ja506:18.3_0": "Sace ca maṁ yācasi etamatthaṁ,",
  "ja506:18.4_0": "Dakkhemu te nāga nivesanāni.",
  "ja506:19.1_0": "Tumhe khottha ghoravisā uḷārā,",
  "ja506:19.2_0": "Mahātejā khippakopī ca hotha;",
  "ja506:19.3_0": "Maṅkāraṇā bandhanasmā pamutto,",
  "ja506:19.4_0": "Arahasi no jānituye katāni”.",
  "ja506:20.1_0": "“So paccataṁ niraye ghorarūpe,",
  "ja506:20.2_0": "Mā kāyikaṁ sātamalattha kiñci;",
  "ja506:20.3_0": "Peḷāya baddho maraṇaṁ upetu,",
  "ja506:20.4_0": "Yo tādisaṁ kammakataṁ na jāne”.",
  "ja506:21.1_0": "“Saccappaṭiññā tava mesa hotu,",
  "ja506:21.2_0": "Akkodhano hohi anupanāhī;",
  "ja506:21.3_0": "Sabbañca te nāgakulaṁ supaṇṇā,",
  "ja506:21.4_0": "Aggiṁva gimhesu vivajjayantu”.",
  "ja506:22.1_0": "“Anukampasī nāgakulaṁ janinda,",
  "ja506:22.2_0": "Mātā yathā suppiyaṁ ekaputtaṁ;",
  "ja506:22.3_0": "Ahañca te nāgakulena saddhiṁ,",
  "ja506:22.4_0": "Kāhāmi veyyāvaṭikaṁ uḷāraṁ”.",
  "ja506:23.1_0": "“Yojentu ve rājarathe sucitte,",
  "ja506:23.2_0": "Kambojake assatare sudante;",
  "ja506:23.3_0": "Nāge ca yojentu suvaṇṇakappane,",
  "ja506:23.4_0": "Dakkhemu nāgassa nivesanāni”.",
  "ja506:24.1_0": "Bherī mudiṅgā paṇavā ca saṅkhā,",
  "ja506:24.2_0": "Avajjayiṁsu uggasenassa rañño;",
  "ja506:24.3_0": "Pāyāsi rājā bahusobhamāno,",
  "ja506:24.4_0": "Purakkhato nārigaṇassa majjhe.",
  "ja506:25.1_0": "Suvaṇṇacitakaṁ bhūmiṁ,",
  "ja506:25.2_0": "addakkhi kāsivaḍḍhano;",
  "ja506:25.3_0": "Sovaṇṇamayapāsāde,",
  "ja506:25.4_0": "veḷuriyaphalakatthate.",
  "ja506:26.1_0": "Sa rājā pāvisi byamhaṁ,",
  "ja506:26.2_0": "campeyyassa nivesanaṁ;",
  "ja506:26.3_0": "Ādiccavaṇṇasannibhaṁ,",
  "ja506:26.4_0": "kaṁsavijju pabhassaraṁ.",
  "ja506:27.1_0": "Nānārukkhehi sañchannaṁ,",
  "ja506:27.2_0": "nānāgandhasamīritaṁ;",
  "ja506:27.3_0": "So pāvekkhi kāsirājā,",
  "ja506:27.4_0": "campeyyassa nivesanaṁ.",
  "ja506:28.1_0": "Paviṭṭhasmiṁ kāsiraññe,",
  "ja506:28.2_0": "campeyyassa nivesanaṁ;",
  "ja506:28.3_0": "Dibbā tūriyā pavajjiṁsu,",
  "ja506:28.4_0": "nāgakaññā ca naccisuṁ.",
  "ja506:29.1_0": "Taṁ nāgakaññā caritaṁ gaṇena,",
  "ja506:29.2_0": "Anvāruhī kāsirājā pasanno;",
  "ja506:29.3_0": "Nisīdi sovaṇṇamayamhi pīṭhe,",
  "ja506:29.4_0": "Sāpassaye candanasāralitte.",
  "ja506:30.1_0": "So tattha bhutvā ca atho ramitvā,",
  "ja506:30.2_0": "Campeyyakaṁ kāsirājā avoca;",
  "ja506:30.3_0": "“Vimānaseṭṭhāni imāni tuyhaṁ,",
  "ja506:30.4_0": "Ādiccavaṇṇāni pabhassarāni;",
  "ja506:30.5_0": "Netādisaṁ atthi manussaloke,",
  "ja506:30.6_0": "Kiṁ patthayaṁ nāga tapo karosi.",
  "ja506:31.1_0": "Tā kambukāyūradharā suvatthā,",
  "ja506:31.2_0": "Vaṭṭaṅgulī tambatalūpapannā;",
  "ja506:31.3_0": "Paggayha pāyenti anomavaṇṇā,",
  "ja506:31.4_0": "Netādisaṁ atthi manussaloke;",
  "ja506:31.5_0": "Kiṁ patthayaṁ nāga tapo karosi.",
  "ja506:32.1_0": "Najjo ca temā puthulomamacchā,",
  "ja506:32.2_0": "Āṭā sakuntābhirudā sutitthā;",
  "ja506:32.3_0": "Netādisaṁ atthi manussaloke,",
  "ja506:32.4_0": "Kiṁ patthayaṁ nāga tapo karosi.",
  "ja506:33.1_0": "Koñcā mayūrā diviyā ca haṁsā,",
  "ja506:33.2_0": "Vaggussarā kokilā sampatanti;",
  "ja506:33.3_0": "Netādisaṁ atthi manussaloke,",
  "ja506:33.4_0": "Kiṁ patthayaṁ nāga tapo karosi.",
  "ja506:34.1_0": "Ambā ca sālā tilakā ca jambuyo,",
  "ja506:34.2_0": "Uddālakā pāṭaliyo ca phullā;",
  "ja506:34.3_0": "Netādisaṁ atthi manussaloke,",
  "ja506:34.4_0": "Kiṁ patthayaṁ nāga tapo karosi.",
  "ja506:35.1_0": "Imā ca te pokkharañño samantato,",
  "ja506:35.2_0": "Dibbā ca gandhā satataṁ pavāyanti;",
  "ja506:35.3_0": "Netādisaṁ atthi manussaloke,",
  "ja506:35.4_0": "Kiṁ patthayaṁ nāga tapo karosi”.",
  "ja506:36.1_0": "“Na puttahetu na dhanassa hetu,",
  "ja506:36.2_0": "Na āyuno cāpi janinda hetu;",
  "ja506:36.3_0": "Manussayoniṁ abhipatthayāno,",
  "ja506:36.4_0": "Tasmā parakkamma tapo karomi”.",
  "ja506:37.1_0": "“Tvaṁ lohitakkho vihatantaraṁso,",
  "ja506:37.2_0": "Alaṅkato kappitakesamassu;",
  "ja506:37.3_0": "Surosito lohitacandanena,",
  "ja506:37.4_0": "Gandhabbarājāva disā pabhāsasi.",
  "ja506:38.1_0": "Deviddhipattosi mahānubhāvo,",
  "ja506:38.2_0": "Sabbehi kāmehi samaṅgibhūto;",
  "ja506:38.3_0": "Pucchāmi taṁ nāgarājetamatthaṁ,",
  "ja506:38.4_0": "Seyyo ito kena manussaloko”.",
  "ja506:39.1_0": "“Janinda nāññatra manussalokā,",
  "ja506:39.2_0": "Suddhī va saṁvijjati saṁyamo vā;",
  "ja506:39.3_0": "Ahañca laddhāna manussayoniṁ,",
  "ja506:39.4_0": "Kāhāmi jātimaraṇassa antaṁ”.",
  "ja506:40.1_0": "“Addhā have sevitabbā sapaññā,",
  "ja506:40.2_0": "Bahussutā ye bahuṭhānacintino;",
  "ja506:40.3_0": "Nāriyo ca disvāna tuvañca nāga,",
  "ja506:40.4_0": "Kāhāmi puññāni anappakāni”.",
  "ja506:41.1_0": "“Addhā have sevitabbā sapaññā,",
  "ja506:41.2_0": "Bahussutā ye bahuṭhānacintino;",
  "ja506:41.3_0": "Nāriyo ca disvāna mamañca rāja,",
  "ja506:41.4_0": "Karohi puññāni anappakāni”.",
  "ja506:42.1_0": "“Idañca me jātarūpaṁ pahūtaṁ,",
  "ja506:42.2_0": "Rāsī suvaṇṇassa ca tālamattā;",
  "ja506:42.3_0": "Ito haritvāna suvaṇṇagharāni,",
  "ja506:42.4_0": "Karassu rūpiyapākāraṁ karontu.",
  "ja506:43.1_0": "Muttā ca vāhasahassāni pañca,",
  "ja506:43.2_0": "Veḷuriyamissāni ito haritvā;",
  "ja506:43.3_0": "Antepure bhūmiyaṁ santharantu,",
  "ja506:43.4_0": "Nikkaddamā hehiti nīrajā ca.",
  "ja506:44.1_0": "Etādisaṁ āvasa rājaseṭṭha,",
  "ja506:44.2_0": "Vimānaseṭṭhaṁ bahu sobhamānaṁ;",
  "ja506:44.3_0": "Bārāṇasiṁ nagaraṁ iddhaṁ phītaṁ,",
  "ja506:44.4_0": "Rajjañca kārehi anomapaññā”ti.",
  "ja506:45.1_0": "Campeyyajātakaṁ dasamaṁ."
}