{
  "bv14:0.1_0": "Buddhavaṁsa",
  "bv14:0.2_0": "14. Sujātabuddhavaṁsa",
  "bv14:1.1_0": "“Tattheva maṇḍakappamhi,",
  "bv14:1.2_0": "sujāto nāma nāyako;",
  "bv14:1.3_0": "Sīhahanusabhakkhandho,",
  "bv14:1.4_0": "appameyyo durāsado.",
  "bv14:2.1_0": "Candova vimalo suddho,",
  "bv14:2.2_0": "sataraṁsīva patāpavā;",
  "bv14:2.3_0": "Evaṁ sobhati sambuddho,",
  "bv14:2.4_0": "jalanto siriyā sadā.",
  "bv14:3.1_0": "Pāpuṇitvāna sambuddho,",
  "bv14:3.2_0": "kevalaṁ bodhimuttamaṁ;",
  "bv14:3.3_0": "Sumaṅgalamhi nagare,",
  "bv14:3.4_0": "dhammacakkaṁ pavattayi.",
  "bv14:4.1_0": "Desente pavaraṁ dhammaṁ,",
  "bv14:4.2_0": "Sujāte lokanāyake;",
  "bv14:4.3_0": "Asītikoṭī abhisamiṁsu,",
  "bv14:4.4_0": "Paṭhame dhammadesane.",
  "bv14:5.1_0": "Yadā sujāto amitayaso,",
  "bv14:5.2_0": "Deve vassaṁ upāgami;",
  "bv14:5.3_0": "Sattatiṁsasatasahassānaṁ,",
  "bv14:5.4_0": "Dutiyābhisamayo ahu.",
  "bv14:6.1_0": "Yadā sujāto asamasamo,",
  "bv14:6.2_0": "Upagacchi pitusantikaṁ;",
  "bv14:6.3_0": "Saṭṭhisatasahassānaṁ,",
  "bv14:6.4_0": "Tatiyābhisamayo ahu.",
  "bv14:7.1_0": "Sannipātā tayo āsuṁ,",
  "bv14:7.2_0": "sujātassa mahesino;",
  "bv14:7.3_0": "Khīṇāsavānaṁ vimalānaṁ,",
  "bv14:7.4_0": "santacittāna tādinaṁ.",
  "bv14:8.1_0": "Abhiññābalappattānaṁ,",
  "bv14:8.2_0": "appattānaṁ bhavābhave;",
  "bv14:8.3_0": "Saṭṭhisatasahassāni,",
  "bv14:8.4_0": "paṭhamaṁ sannipatiṁsu te.",
  "bv14:9.1_0": "Punāparaṁ sannipāte,",
  "bv14:9.2_0": "tidivorohaṇe jine;",
  "bv14:9.3_0": "Paññāsasatasahassānaṁ,",
  "bv14:9.4_0": "dutiyo āsi samāgamo.",
  "bv14:10.1_0": "Upasaṅkamanto narāsabhaṁ,",
  "bv14:10.2_0": "Tassa yo aggasāvako;",
  "bv14:10.3_0": "Catūhi satasahassehi,",
  "bv14:10.4_0": "Sambuddhaṁ upasaṅkami.",
  "bv14:11.1_0": "Ahaṁ tena samayena,",
  "bv14:11.2_0": "catudīpamhi issaro;",
  "bv14:11.3_0": "Antalikkhacaro āsiṁ,",
  "bv14:11.4_0": "cakkavattī mahabbalo.",
  "bv14:12.1_0": "Loke acchariyaṁ disvā,",
  "bv14:12.2_0": "abbhutaṁ lomahaṁsanaṁ;",
  "bv14:12.3_0": "Upagantvāna vandiṁ so,",
  "bv14:12.4_0": "sujātaṁ lokanāyakaṁ.",
  "bv14:13.1_0": "Catudīpe mahārajjaṁ,",
  "bv14:13.2_0": "ratane satta uttame;",
  "bv14:13.3_0": "Buddhe niyyādayitvāna,",
  "bv14:13.4_0": "pabbajiṁ tassa santike.",
  "bv14:14.1_0": "Ārāmikā janapade,",
  "bv14:14.2_0": "uṭṭhānaṁ paṭipiṇḍiya;",
  "bv14:14.3_0": "Upanenti bhikkhusaṅghassa,",
  "bv14:14.4_0": "paccayaṁ sayanāsanaṁ.",
  "bv14:15.1_0": "Sopi maṁ buddho byākāsi,",
  "bv14:15.2_0": "dasasahassimhi issaro;",
  "bv14:15.3_0": "‘Tiṁsakappasahassamhi,",
  "bv14:15.4_0": "ayaṁ buddho bhavissati.",
  "bv14:16.1_0": "Padhānaṁ padahitvāna,",
  "bv14:16.2_0": "…pe…",
  "bv14:16.3_0": "hessāma sammukhā imaṁ’.",
  "bv14:17.1_0": "Tassāpi vacanaṁ sutvā,",
  "bv14:17.2_0": "bhiyyo hāsaṁ janesahaṁ;",
  "bv14:17.3_0": "Adhiṭṭhahiṁ vataṁ uggaṁ,",
  "bv14:17.4_0": "dasapāramipūriyā.",
  "bv14:18.1_0": "Suttantaṁ vinayañcāpi,",
  "bv14:18.2_0": "navaṅgaṁ satthusāsanaṁ;",
  "bv14:18.3_0": "Sabbaṁ pariyāpuṇitvāna,",
  "bv14:18.4_0": "sobhayiṁ jinasāsanaṁ.",
  "bv14:19.1_0": "Tatthappamatto viharanto,",
  "bv14:19.2_0": "brahmaṁ bhāvetva bhāvanaṁ;",
  "bv14:19.3_0": "Abhiññāpāramiṁ gantvā,",
  "bv14:19.4_0": "brahmalokamagañchahaṁ.",
  "bv14:20.1_0": "Sumaṅgalaṁ nāma nagaraṁ,",
  "bv14:20.2_0": "uggato nāma khattiyo;",
  "bv14:20.3_0": "Mātā pabhāvatī nāma,",
  "bv14:20.4_0": "sujātassa mahesino.",
  "bv14:21.1_0": "Navavassasahassāni,",
  "bv14:21.2_0": "agāraṁ ajjha so vasi;",
  "bv14:21.3_0": "Sirī upasirī nando,",
  "bv14:21.4_0": "tayo pāsādamuttamā.",
  "bv14:22.1_0": "Tevīsatisahassāni,",
  "bv14:22.2_0": "nāriyo samalaṅkatā;",
  "bv14:22.3_0": "Sirinandā nāma nārī,",
  "bv14:22.4_0": "upaseno nāma atrajo.",
  "bv14:23.1_0": "Nimitte caturo disvā,",
  "bv14:23.2_0": "assayānena nikkhami;",
  "bv14:23.3_0": "Anūnanavamāsāni,",
  "bv14:23.4_0": "padhānaṁ padahī jino.",
  "bv14:24.1_0": "Brahmunā yācito santo,",
  "bv14:24.2_0": "sujāto lokanāyako;",
  "bv14:24.3_0": "Vatti cakkaṁ mahāvīro,",
  "bv14:24.4_0": "sumaṅgaluyyānamuttame.",
  "bv14:25.1_0": "Sudassano sudevo ca,",
  "bv14:25.2_0": "ahesuṁ aggasāvakā;",
  "bv14:25.3_0": "Nārado nāmupaṭṭhāko,",
  "bv14:25.4_0": "sujātassa mahesino.",
  "bv14:26.1_0": "Nāgā ca nāgasamālā ca,",
  "bv14:26.2_0": "ahesuṁ aggasāvikā;",
  "bv14:26.3_0": "Bodhi tassa bhagavato,",
  "bv14:26.4_0": "mahāveḷūti vuccati.",
  "bv14:27.1_0": "So ca rukkho ghanakkhandho,",
  "bv14:27.2_0": "Acchiddo hoti pattiko;",
  "bv14:27.3_0": "Uju vaṁso brahā hoti,",
  "bv14:27.4_0": "Dassanīyo manoramo.",
  "bv14:28.1_0": "Ekakkhandho pavaḍḍhitvā,",
  "bv14:28.2_0": "tato sākhā pabhijjati;",
  "bv14:28.3_0": "Yathā subaddho morahattho,",
  "bv14:28.4_0": "evaṁ sobhati so dumo.",
  "bv14:29.1_0": "Na tassa kaṇṭakā honti,",
  "bv14:29.2_0": "nāpi chiddaṁ mahā ahu;",
  "bv14:29.3_0": "Vitthiṇṇasākho aviralo,",
  "bv14:29.4_0": "sandacchāyo manoramo.",
  "bv14:30.1_0": "Sudatto ceva citto ca,",
  "bv14:30.2_0": "ahesuṁ aggupaṭṭhakā;",
  "bv14:30.3_0": "Subhaddā ca padumā ca,",
  "bv14:30.4_0": "ahesuṁ aggupaṭṭhikā.",
  "bv14:31.1_0": "Paññāsaratano āsi,",
  "bv14:31.2_0": "uccattanena so jino;",
  "bv14:31.3_0": "Sabbākāravarūpeto,",
  "bv14:31.4_0": "sabbaguṇamupāgato.",
  "bv14:32.1_0": "Tassa pabhā asamasamā,",
  "bv14:32.2_0": "niddhāvati samantato;",
  "bv14:32.3_0": "Appamāṇo atuliyo,",
  "bv14:32.4_0": "opammehi anūpamo.",
  "bv14:33.1_0": "Navutivassasahassāni,",
  "bv14:33.2_0": "āyu vijjati tāvade;",
  "bv14:33.3_0": "Tāvatā tiṭṭhamāno so,",
  "bv14:33.4_0": "tāresi janataṁ bahuṁ.",
  "bv14:34.1_0": "Yathāpi sāgare ūmī,",
  "bv14:34.2_0": "gagane tārakā yathā;",
  "bv14:34.3_0": "Evaṁ tadā pāvacanaṁ,",
  "bv14:34.4_0": "arahantehi cittitaṁ.",
  "bv14:35.1_0": "So ca buddho asamasamo,",
  "bv14:35.2_0": "Guṇāni ca tāni atuliyāni;",
  "bv14:35.3_0": "Sabbaṁ tamantarahitaṁ,",
  "bv14:35.4_0": "Nanu rittā sabbasaṅkhārā.",
  "bv14:36.1_0": "Sujāto jinavaro buddho,",
  "bv14:36.2_0": "Silārāmamhi nibbuto;",
  "bv14:36.3_0": "Tattheva tassa cetiyo,",
  "bv14:36.4_0": "Tīṇigāvutamuggato”ti.",
  "bv14:37.1_0": "Sujātassa bhagavato vaṁso dvādasamo."
}