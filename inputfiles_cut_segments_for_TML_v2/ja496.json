{
  "ja496:0.1_0": "Jātaka",
  "ja496:0.2_0": "Pakiṇṇakanipāta",
  "ja496:0.3_0": "Sālikedāravagga",
  "ja496:0.4_0": "13. Bhikkhāparamparajātaka",
  "ja496:1.1_0": "“Sukhumālarūpaṁ disvā,",
  "ja496:1.2_0": "raṭṭhā vivanamāgataṁ;",
  "ja496:1.3_0": "Kūṭāgāravarūpetaṁ,",
  "ja496:1.4_0": "mahāsayanamupāsitaṁ.",
  "ja496:2.1_0": "Tassa te pemakenāhaṁ,",
  "ja496:2.2_0": "adāsiṁ vaḍḍhamodanaṁ;",
  "ja496:2.3_0": "Sālīnaṁ vicitaṁ bhattaṁ,",
  "ja496:2.4_0": "suciṁ maṁsūpasecanaṁ.",
  "ja496:3.1_0": "Taṁ tvaṁ bhattaṁ paṭiggayha,",
  "ja496:3.2_0": "brāhmaṇassa adāsayi;",
  "ja496:3.3_0": "Attānaṁ anasitvāna,",
  "ja496:3.4_0": "koyaṁ dhammo namatthu te”.",
  "ja496:4.1_0": "“Ācariyo brāhmaṇo mayhaṁ,",
  "ja496:4.2_0": "kiccākiccesu byāvaṭo;",
  "ja496:4.3_0": "Garu ca āmantanīyo ca,",
  "ja496:4.4_0": "dātumarahāmi bhojanaṁ”.",
  "ja496:5.1_0": "“Brāhmaṇaṁ dāni pucchāmi,",
  "ja496:5.2_0": "gotamaṁ rājapūjitaṁ;",
  "ja496:5.3_0": "Rājā te bhattaṁ pādāsi,",
  "ja496:5.4_0": "suciṁ maṁsūpasecanaṁ.",
  "ja496:6.1_0": "Taṁ tvaṁ bhattaṁ paṭiggayha,",
  "ja496:6.2_0": "isissa bhojanaṁ adā;",
  "ja496:6.3_0": "Akhettaññūsi dānassa,",
  "ja496:6.4_0": "koyaṁ dhammo namatthu te”.",
  "ja496:7.1_0": "“Bharāmi puttadāre ca,",
  "ja496:7.2_0": "gharesu gadhito ahaṁ;",
  "ja496:7.3_0": "Bhuñje mānusake kāme,",
  "ja496:7.4_0": "anusāsāmi rājino.",
  "ja496:8.1_0": "Āraññikassa isino,",
  "ja496:8.2_0": "cirarattaṁ tapassino;",
  "ja496:8.3_0": "Vuḍḍhassa bhāvitattassa,",
  "ja496:8.4_0": "dātumarahāmi bhojanaṁ”.",
  "ja496:9.1_0": "“Isiñca dāni pucchāmi,",
  "ja496:9.2_0": "kisaṁ dhamanisanthataṁ;",
  "ja496:9.3_0": "Parūḷhakacchanakhalomaṁ,",
  "ja496:9.4_0": "paṅkadantaṁ rajassiraṁ.",
  "ja496:10.1_0": "Eko araññe viharasi,",
  "ja496:10.2_0": "nāvakaṅkhasi jīvitaṁ;",
  "ja496:10.3_0": "Bhikkhu kena tayā seyyo,",
  "ja496:10.4_0": "yassa tvaṁ bhojanaṁ adā”.",
  "ja496:11.1_0": "“Khaṇantālukalambāni,",
  "ja496:11.2_0": "bilālitakkalāni ca;",
  "ja496:11.3_0": "Dhunaṁ sāmākanīvāraṁ,",
  "ja496:11.4_0": "saṅghāriyaṁ pasāriyaṁ.",
  "ja496:12.1_0": "Sākaṁ bhisaṁ madhuṁ maṁsaṁ,",
  "ja496:12.2_0": "Badarāmalakāni ca;",
  "ja496:12.3_0": "Tāni āharitvā bhuñjāmi,",
  "ja496:12.4_0": "Atthi me so pariggaho.",
  "ja496:13.1_0": "Pacanto apacantassa,",
  "ja496:13.2_0": "amamassa sakiñcano;",
  "ja496:13.3_0": "Anādānassa sādāno,",
  "ja496:13.4_0": "dātumarahāmi bhojanaṁ”.",
  "ja496:14.1_0": "“Bhikkhuñca dāni pucchāmi,",
  "ja496:14.2_0": "tuṇhīmāsīna subbataṁ;",
  "ja496:14.3_0": "Isi te bhattaṁ pādāsi,",
  "ja496:14.4_0": "suciṁ maṁsūpasecanaṁ.",
  "ja496:15.1_0": "Taṁ tvaṁ bhattaṁ paṭiggayha,",
  "ja496:15.2_0": "tuṇhī bhuñjasi ekako;",
  "ja496:15.3_0": "Nāññaṁ kañci nimantesi,",
  "ja496:15.4_0": "koyaṁ dhammo namatthu te”.",
  "ja496:16.1_0": "“Na pacāmi na pācemi,",
  "ja496:16.2_0": "na chindāmi na chedaye;",
  "ja496:16.3_0": "Taṁ maṁ akiñcanaṁ ñatvā,",
  "ja496:16.4_0": "sabbapāpehi ārataṁ.",
  "ja496:17.1_0": "Vāmena bhikkhamādāya,",
  "ja496:17.2_0": "dakkhiṇena kamaṇḍaluṁ;",
  "ja496:17.3_0": "Isi me bhattaṁ pādāsi,",
  "ja496:17.4_0": "suciṁ maṁsūpasecanaṁ.",
  "ja496:18.1_0": "Ete hi dātumarahanti,",
  "ja496:18.2_0": "samamā sapariggahā;",
  "ja496:18.3_0": "Paccanīkamahaṁ maññe,",
  "ja496:18.4_0": "yo dātāraṁ nimantaye”.",
  "ja496:19.1_0": "“Atthāya vata me ajja,",
  "ja496:19.2_0": "idhāgacchi rathesabho;",
  "ja496:19.3_0": "Sohaṁ ajja pajānāmi,",
  "ja496:19.4_0": "yattha dinnaṁ mahapphalaṁ.",
  "ja496:20.1_0": "Raṭṭhesu giddhā rājāno,",
  "ja496:20.2_0": "Kiccākiccesu brāhmaṇā;",
  "ja496:20.3_0": "Isī mūlaphale giddhā,",
  "ja496:20.4_0": "Vippamuttā ca bhikkhavo”ti.",
  "ja496:21.1_0": "Bhikkhāparamparajātakaṁ terasamaṁ.",
  "ja496:22.1_0": "Pakiṇṇakanipātaṁ niṭṭhitaṁ.",
  "ja496:23.1_0": "Tassuddānaṁ",
  "ja496:24.1_0": "Suva kinnara mukka kharājinaso,",
  "ja496:24.2_0": "Bhisajāta mahesi kapotavaro;",
  "ja496:24.3_0": "Atha mora satacchaka vāṇijako,",
  "ja496:24.4_0": "Atha rāja sabrāhmaṇa bhikkhaparanti."
}