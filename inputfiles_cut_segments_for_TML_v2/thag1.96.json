{
  "thag1.96:0.1_0": "Theragāthā",
  "thag1.96:0.2_0": "Ekakanipāta",
  "thag1.96:0.3_0": "Dasamavagga",
  "thag1.96:0.4_0": "1.96. Khaṇḍasumanattheragāthā",
  "thag1.96:1.1_0": "“Ekapupphaṁ cajitvāna,",
  "thag1.96:1.2_0": "asīti vassakoṭiyo;",
  "thag1.96:1.3_0": "Saggesu paricāretvā,",
  "thag1.96:1.4_0": "sesakenamhi nibbuto”ti.",
  "thag1.96:1.5_0": "… Khaṇḍasumano thero …."
}