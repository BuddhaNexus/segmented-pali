{
  "tha-ap558:0.1_0": "Therāpadāna",
  "tha-ap558:0.2_0": "Yasavagga",
  "tha-ap558:0.3_0": "6. Uttarattheraapadāna",
  "tha-ap558:1.1_0": "“Sumedho nāma sambuddho,",
  "tha-ap558:1.2_0": "bāttiṁsavaralakkhaṇo;",
  "tha-ap558:1.3_0": "Vivekakāmo bhagavā,",
  "tha-ap558:1.4_0": "himavantamupāgami.",
  "tha-ap558:2.1_0": "Ajjhogāhetvā himavantaṁ,",
  "tha-ap558:2.2_0": "aggo kāruṇiko muni;",
  "tha-ap558:2.3_0": "Pallaṅkaṁ ābhujitvāna,",
  "tha-ap558:2.4_0": "nisīdi purisuttamo.",
  "tha-ap558:3.1_0": "Vijjādharo tadā āsiṁ,",
  "tha-ap558:3.2_0": "antalikkhacaro ahaṁ;",
  "tha-ap558:3.3_0": "Tisūlaṁ sukataṁ gayha,",
  "tha-ap558:3.4_0": "gacchāmi ambare tadā.",
  "tha-ap558:4.1_0": "Pabbatagge yathā aggi,",
  "tha-ap558:4.2_0": "puṇṇamāyeva candimā;",
  "tha-ap558:4.3_0": "Vanaṁ obhāsate buddho,",
  "tha-ap558:4.4_0": "sālarājāva phullito.",
  "tha-ap558:5.1_0": "Vanaggā nikkhamitvāna,",
  "tha-ap558:5.2_0": "buddharaṁsībhidhāvare;",
  "tha-ap558:5.3_0": "Naḷaggivaṇṇasaṅkāsā,",
  "tha-ap558:5.4_0": "disvā cittaṁ pasādayiṁ.",
  "tha-ap558:6.1_0": "Vicinaṁ addasaṁ pupphaṁ,",
  "tha-ap558:6.2_0": "kaṇikāraṁ devagandhikaṁ;",
  "tha-ap558:6.3_0": "Tīṇi pupphāni ādāya,",
  "tha-ap558:6.4_0": "buddhaseṭṭhamapūjayiṁ.",
  "tha-ap558:7.1_0": "Buddhassa ānubhāvena,",
  "tha-ap558:7.2_0": "tīṇi pupphāni me tadā;",
  "tha-ap558:7.3_0": "Uddhaṁ vaṇṭā adhopattā,",
  "tha-ap558:7.4_0": "chāyaṁ kubbanti satthuno.",
  "tha-ap558:8.1_0": "Tena kammena sukatena,",
  "tha-ap558:8.2_0": "cetanāpaṇidhīhi ca;",
  "tha-ap558:8.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "tha-ap558:8.4_0": "tāvatiṁsamagacchahaṁ.",
  "tha-ap558:9.1_0": "Tattha me sukataṁ byamhaṁ,",
  "tha-ap558:9.2_0": "kaṇikārīti ñāyati;",
  "tha-ap558:9.3_0": "Saṭṭhiyojanamubbedhaṁ,",
  "tha-ap558:9.4_0": "tiṁsayojanavitthataṁ.",
  "tha-ap558:10.1_0": "Sahassakaṇḍaṁ satabheṇḍu,",
  "tha-ap558:10.2_0": "dhajālu haritāmayaṁ;",
  "tha-ap558:10.3_0": "Satasahassaniyyūhā,",
  "tha-ap558:10.4_0": "byamhe pātubhaviṁsu me.",
  "tha-ap558:11.1_0": "Soṇṇamayā maṇimayā,",
  "tha-ap558:11.2_0": "lohitaṅkamayāpi ca;",
  "tha-ap558:11.3_0": "Phalikāpi ca pallaṅkā,",
  "tha-ap558:11.4_0": "yenicchakā yadicchakā.",
  "tha-ap558:12.1_0": "Mahārahañca sayanaṁ,",
  "tha-ap558:12.2_0": "tūlikā vikatīyutaṁ;",
  "tha-ap558:12.3_0": "Uddhalomiñca ekantaṁ,",
  "tha-ap558:12.4_0": "bimbohanasamāyutaṁ.",
  "tha-ap558:13.1_0": "Bhavanā nikkhamitvāna,",
  "tha-ap558:13.2_0": "caranto devacārikaṁ;",
  "tha-ap558:13.3_0": "Yathā icchāmi gamanaṁ,",
  "tha-ap558:13.4_0": "devasaṅghapurakkhato.",
  "tha-ap558:14.1_0": "Pupphassa heṭṭhā tiṭṭhāmi,",
  "tha-ap558:14.2_0": "uparicchadanaṁ mama;",
  "tha-ap558:14.3_0": "Samantā yojanasataṁ,",
  "tha-ap558:14.4_0": "kaṇikārehi chāditaṁ.",
  "tha-ap558:15.1_0": "Saṭṭhituriyasahassāni,",
  "tha-ap558:15.2_0": "sāyapātaṁ upaṭṭhahuṁ;",
  "tha-ap558:15.3_0": "Parivārenti maṁ niccaṁ,",
  "tha-ap558:15.4_0": "rattindivamatanditā.",
  "tha-ap558:16.1_0": "Tattha naccehi gītehi,",
  "tha-ap558:16.2_0": "tālehi vāditehi ca;",
  "tha-ap558:16.3_0": "Ramāmi khiḍḍā ratiyā,",
  "tha-ap558:16.4_0": "modāmi kāmakāmahaṁ.",
  "tha-ap558:17.1_0": "Tattha bhutvā pivitvā ca,",
  "tha-ap558:17.2_0": "modāmi tidase tadā;",
  "tha-ap558:17.3_0": "Nārīgaṇehi sahito,",
  "tha-ap558:17.4_0": "modāmi byamhamuttame.",
  "tha-ap558:18.1_0": "Satānaṁ pañcakkhattuñca,",
  "tha-ap558:18.2_0": "devarajjamakārayiṁ;",
  "tha-ap558:18.3_0": "Satānaṁ tīṇikkhattuñca,",
  "tha-ap558:18.4_0": "cakkavattī ahosahaṁ;",
  "tha-ap558:18.5_0": "Padesarajjaṁ vipulaṁ,",
  "tha-ap558:18.6_0": "gaṇanāto asaṅkhiyaṁ.",
  "tha-ap558:19.1_0": "Bhave bhave saṁsaranto,",
  "tha-ap558:19.2_0": "mahābhogaṁ labhāmahaṁ;",
  "tha-ap558:19.3_0": "Bhoge me ūnatā natthi,",
  "tha-ap558:19.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap558:20.1_0": "Duve bhave saṁsarāmi,",
  "tha-ap558:20.2_0": "devatte atha mānuse;",
  "tha-ap558:20.3_0": "Aññaṁ gatiṁ na jānāmi,",
  "tha-ap558:20.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap558:21.1_0": "Duve kule pajāyāmi,",
  "tha-ap558:21.2_0": "khattiye cāpi brāhmaṇe;",
  "tha-ap558:21.3_0": "Nīce kule na jānāmi,",
  "tha-ap558:21.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap558:22.1_0": "Hatthiyānaṁ assayānaṁ,",
  "tha-ap558:22.2_0": "sivikaṁ sandamānikaṁ;",
  "tha-ap558:22.3_0": "Labhāmi sabbamevetaṁ,",
  "tha-ap558:22.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap558:23.1_0": "Dāsīgaṇaṁ dāsagaṇaṁ,",
  "tha-ap558:23.2_0": "nāriyo samalaṅkatā;",
  "tha-ap558:23.3_0": "Labhāmi sabbamevetaṁ,",
  "tha-ap558:23.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap558:24.1_0": "Koseyyakambaliyāni,",
  "tha-ap558:24.2_0": "khomakappāsikāni ca;",
  "tha-ap558:24.3_0": "Labhāmi sabbamevetaṁ,",
  "tha-ap558:24.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap558:25.1_0": "Navavatthaṁ navaphalaṁ,",
  "tha-ap558:25.2_0": "navaggarasabhojanaṁ;",
  "tha-ap558:25.3_0": "Labhāmi sabbamevetaṁ,",
  "tha-ap558:25.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap558:26.1_0": "Imaṁ khāda imaṁ bhuñja,",
  "tha-ap558:26.2_0": "imamhi sayane saya;",
  "tha-ap558:26.3_0": "Labhāmi sabbamevetaṁ,",
  "tha-ap558:26.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap558:27.1_0": "Sabbattha pūjito homi,",
  "tha-ap558:27.2_0": "yaso accuggato mama;",
  "tha-ap558:27.3_0": "Mahāpakkho sadā homi,",
  "tha-ap558:27.4_0": "abhejjapariso sadā;",
  "tha-ap558:27.5_0": "Ñātīnaṁ uttamo homi,",
  "tha-ap558:27.6_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap558:28.1_0": "Sītaṁ uṇhaṁ na jānāmi,",
  "tha-ap558:28.2_0": "pariḷāho na vijjati;",
  "tha-ap558:28.3_0": "Atho cetasikaṁ dukkhaṁ,",
  "tha-ap558:28.4_0": "hadaye me na vijjati.",
  "tha-ap558:29.1_0": "Suvaṇṇavaṇṇo hutvāna,",
  "tha-ap558:29.2_0": "saṁsarāmi bhavābhave;",
  "tha-ap558:29.3_0": "Vevaṇṇiyaṁ na jānāmi,",
  "tha-ap558:29.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap558:30.1_0": "Devalokā cavitvāna,",
  "tha-ap558:30.2_0": "sukkamūlena codito;",
  "tha-ap558:30.3_0": "Sāvatthiyaṁ pure jāto,",
  "tha-ap558:30.4_0": "mahāsālesu aḍḍhake.",
  "tha-ap558:31.1_0": "Pañca kāmaguṇe hitvā,",
  "tha-ap558:31.2_0": "pabbajiṁ anagāriyaṁ;",
  "tha-ap558:31.3_0": "Jātiyā sattavassohaṁ,",
  "tha-ap558:31.4_0": "arahattamapāpuṇiṁ.",
  "tha-ap558:32.1_0": "Upasampadāyī buddho,",
  "tha-ap558:32.2_0": "guṇamaññāya cakkhumā;",
  "tha-ap558:32.3_0": "Taruṇo pūjanīyohaṁ,",
  "tha-ap558:32.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap558:33.1_0": "Dibbacakkhuvisuddhaṁ me,",
  "tha-ap558:33.2_0": "samādhikusalo ahaṁ;",
  "tha-ap558:33.3_0": "Abhiññāpāramippatto,",
  "tha-ap558:33.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap558:34.1_0": "Paṭisambhidā anuppatto,",
  "tha-ap558:34.2_0": "iddhipādesu kovido;",
  "tha-ap558:34.3_0": "Dhammesu pāramippatto,",
  "tha-ap558:34.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap558:35.1_0": "Tiṁsakappasahassamhi,",
  "tha-ap558:35.2_0": "yaṁ buddhamabhipūjayiṁ;",
  "tha-ap558:35.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap558:35.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap558:36.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap558:36.2_0": "…pe…",
  "tha-ap558:36.3_0": "viharāmi anāsavo.",
  "tha-ap558:37.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap558:37.2_0": "…pe…",
  "tha-ap558:37.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap558:38.1_0": "Paṭisambhidā catasso,",
  "tha-ap558:38.2_0": "…pe…",
  "tha-ap558:38.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap558:39.1_0": "Itthaṁ sudaṁ āyasmā uttaro thero imā gāthāyo abhāsitthāti.",
  "tha-ap558:40.1_0": "Uttarattherassāpadānaṁ chaṭṭhaṁ."
}