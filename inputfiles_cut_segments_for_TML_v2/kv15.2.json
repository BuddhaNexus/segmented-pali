{
  "kv15.2:0.1_0": "Kathāvatthu",
  "kv15.2:0.2_0": "Mahāpaṇṇāsaka",
  "kv15.2:0.3_0": "Pannarasamavagga",
  "kv15.2:0.4_0": "Aññamaññapaccayakathā",
  "kv15.2:1.1_0": "Avijjāpaccayāva saṅkhārā, na vattabbaṁ—",
  "kv15.2:1.2_0": "“saṅkhārapaccayāpi avijjā”ti?",
  "kv15.2:1.3_0": "Āmantā.",
  "kv15.2:1.4_0": "Nanu avijjā saṅkhārena sahajātāti?",
  "kv15.2:1.5_0": "Āmantā.",
  "kv15.2:1.6_0": "Hañci avijjā saṅkhārena sahajātā, tena vata re vattabbe—",
  "kv15.2:1.7_0": "“avijjāpaccayāpi saṅkhārā, saṅkhārapaccayāpi avijjā”ti.",
  "kv15.2:2.1_0": "Taṇhāpaccayāva upādānaṁ, na vattabbaṁ—",
  "kv15.2:2.2_0": "“upādānapaccayāpi taṇhā”ti?",
  "kv15.2:2.3_0": "Āmantā.",
  "kv15.2:2.4_0": "Nanu taṇhā upādānena sahajātāti?",
  "kv15.2:2.5_0": "Āmantā.",
  "kv15.2:2.6_0": "Hañci taṇhā upādānena sahajātā, tena vata re vattabbe—",
  "kv15.2:2.7_0": "“taṇhāpaccayāpi upādānaṁ, upādānapaccayāpi taṇhā”ti.",
  "kv15.2:3.1_0": "“Jarāmaraṇapaccayā, bhikkhave, jāti, jātipaccayā bhavo”ti—",
  "kv15.2:3.2_0": "attheva suttantoti?",
  "kv15.2:3.3_0": "Natthi.",
  "kv15.2:3.4_0": "Tena hi avijjāpaccayāva saṅkhārā, na vattabbaṁ—",
  "kv15.2:3.5_0": "“saṅkhārapaccayāpi avijjā”ti.",
  "kv15.2:3.6_0": "Taṇhāpaccayāva upādānaṁ, na vattabbaṁ—",
  "kv15.2:3.7_0": "“upādānapaccayāpi taṇhā”ti.",
  "kv15.2:4.1_0": "“Viññāṇapaccayā, bhikkhave, nāmarūpaṁ, nāmarūpapaccayāpi viññāṇan”ti—",
  "kv15.2:4.2_0": "attheva suttantoti?",
  "kv15.2:4.3_0": "Āmantā.",
  "kv15.2:4.4_0": "Tena hi avijjāpaccayāpi saṅkhārā, saṅkhārapaccayāpi avijjā;",
  "kv15.2:4.5_0": "taṇhāpaccayāpi upādānaṁ, upādānapaccayāpi taṇhāti.",
  "kv15.2:4.6_0": "Aññamaññapaccayakathā niṭṭhitā."
}