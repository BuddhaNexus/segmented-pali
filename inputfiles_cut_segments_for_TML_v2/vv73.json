{
  "vv73:0.1_0": "Vimānavatthu",
  "vv73:0.2_0": "Purisavimāna",
  "vv73:0.3_0": "Pāyāsivagga",
  "vv73:0.4_0": "9. Dutiyakuṇḍalīvimānavatthu",
  "vv73:1.1_0": "“Alaṅkato malyadharo suvattho,",
  "vv73:1.2_0": "Sukuṇḍalī kappitakesamassu;",
  "vv73:1.3_0": "Āmuttahatthābharaṇo yasassī,",
  "vv73:1.4_0": "Dibbe vimānamhi yathāpi candimā.",
  "vv73:2.1_0": "Dibbā ca vīṇā pavadanti vagguṁ,",
  "vv73:2.2_0": "Aṭṭhaṭṭhakā sikkhitā sādhurūpā;",
  "vv73:2.3_0": "Dibbā ca kaññā tidasacarā uḷārā,",
  "vv73:2.4_0": "Naccanti gāyanti pamodayanti.",
  "vv73:3.1_0": "Deviddhipattosi mahānubhāvo,",
  "vv73:3.2_0": "Manussabhūto kimakāsi puññaṁ;",
  "vv73:3.3_0": "Kenāsi evaṁ jalitānubhāvo,",
  "vv73:3.4_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv73:4.1_0": "So devaputto attamano,",
  "vv73:4.2_0": "…pe…",
  "vv73:4.3_0": "yassa kammassidaṁ phalaṁ.",
  "vv73:5.1_0": "“Ahaṁ manussesu manussabhūto,",
  "vv73:5.2_0": "Disvāna samaṇe sādhurūpe;",
  "vv73:5.3_0": "Sampannavijjācaraṇe yasassī,",
  "vv73:5.4_0": "Bahussute sīlavante pasanne;",
  "vv73:5.5_0": "Annañca pānañca pasannacitto,",
  "vv73:5.6_0": "Sakkacca dānaṁ vipulaṁ adāsiṁ.",
  "vv73:6.1_0": "Tena metādiso vaṇṇo,",
  "vv73:6.2_0": "…pe…",
  "vv73:6.3_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv73:7.1_0": "Dutiyakuṇḍalīvimānaṁ navamaṁ."
}