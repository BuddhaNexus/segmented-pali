{
  "vv38:0.1_0": "Vimānavatthu",
  "vv38:0.2_0": "Itthivimāna",
  "vv38:0.3_0": "Pāricchattakavagga",
  "vv38:0.4_0": "10. Pāricchattakavimānavatthu",
  "vv38:1.1_0": "“Pāricchattake koviḷāre,",
  "vv38:1.2_0": "ramaṇīye manorame;",
  "vv38:1.3_0": "Dibbamālaṁ ganthamānā,",
  "vv38:1.4_0": "gāyantī sampamodasi.",
  "vv38:2.1_0": "Tassā te naccamānāya,",
  "vv38:2.2_0": "aṅgamaṅgehi sabbaso;",
  "vv38:2.3_0": "Dibbā saddā niccharanti,",
  "vv38:2.4_0": "savanīyā manoramā.",
  "vv38:3.1_0": "Tassā te naccamānāya,",
  "vv38:3.2_0": "aṅgamaṅgehi sabbaso;",
  "vv38:3.3_0": "Dibbā gandhā pavāyanti,",
  "vv38:3.4_0": "sucigandhā manoramā.",
  "vv38:4.1_0": "Vivattamānā kāyena,",
  "vv38:4.2_0": "yā veṇīsu piḷandhanā;",
  "vv38:4.3_0": "Tesaṁ suyyati nigghoso,",
  "vv38:4.4_0": "tūriye pañcaṅgike yathā.",
  "vv38:5.1_0": "Vaṭaṁsakā vātadhutā,",
  "vv38:5.2_0": "vātena sampakampitā;",
  "vv38:5.3_0": "Tesaṁ suyyati nigghoso,",
  "vv38:5.4_0": "tūriye pañcaṅgike yathā.",
  "vv38:6.1_0": "Yāpi te sirasmiṁ mālā,",
  "vv38:6.2_0": "sucigandhā manoramā;",
  "vv38:6.3_0": "Vāti gandho disā sabbā,",
  "vv38:6.4_0": "rukkho mañjūsako yathā.",
  "vv38:7.1_0": "Ghāyase taṁ sucigandhaṁ,",
  "vv38:7.2_0": "rūpaṁ passasi amānusaṁ;",
  "vv38:7.3_0": "Devate pucchitācikkha,",
  "vv38:7.4_0": "kissa kammassidaṁ phalan”ti.",
  "vv38:8.1_0": "“Pabhassaraṁ accimantaṁ,",
  "vv38:8.2_0": "vaṇṇagandhena saṁyutaṁ;",
  "vv38:8.3_0": "Asokapupphamālāhaṁ,",
  "vv38:8.4_0": "buddhassa upanāmayiṁ.",
  "vv38:9.1_0": "Tāhaṁ kammaṁ karitvāna,",
  "vv38:9.2_0": "kusalaṁ buddhavaṇṇitaṁ;",
  "vv38:9.3_0": "Apetasokā sukhitā,",
  "vv38:9.4_0": "sampamodāmanāmayā”ti.",
  "vv38:10.1_0": "Pāricchattakavimānaṁ dasamaṁ.",
  "vv38:11.1_0": "Tassuddānaṁ",
  "vv38:12.1_0": "Uḷāro ucchu pallaṅko,",
  "vv38:12.2_0": "latā ca guttilena ca;",
  "vv38:12.3_0": "Daddallapesamallikā,",
  "vv38:12.4_0": "visālakkhi pāricchattako;",
  "vv38:12.5_0": "Vaggo tena pavuccatīti.",
  "vv38:13.1_0": "Pāricchattakavaggo tatiyo."
}