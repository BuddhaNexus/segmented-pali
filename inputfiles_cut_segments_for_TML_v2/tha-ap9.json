{
  "tha-ap9:0.1_0": "Therāpadāna",
  "tha-ap9:0.2_0": "Buddhavagga",
  "tha-ap9:0.3_0": "9 Aññāsikoṇḍaññattheraapadāna",
  "tha-ap9:1.1_0": "“Padumuttarasambuddhaṁ,",
  "tha-ap9:1.2_0": "lokajeṭṭhaṁ vināyakaṁ;",
  "tha-ap9:1.3_0": "Buddhabhūmimanuppattaṁ,",
  "tha-ap9:1.4_0": "paṭhamaṁ addasaṁ ahaṁ.",
  "tha-ap9:2.1_0": "Yāvatā bodhiyā mūle,",
  "tha-ap9:2.2_0": "yakkhā sabbe samāgatā;",
  "tha-ap9:2.3_0": "Sambuddhaṁ parivāretvā,",
  "tha-ap9:2.4_0": "vandanti pañjalīkatā.",
  "tha-ap9:3.1_0": "Sabbe devā tuṭṭhamanā,",
  "tha-ap9:3.2_0": "ākāse sañcaranti te;",
  "tha-ap9:3.3_0": "Buddho ayaṁ anuppatto,",
  "tha-ap9:3.4_0": "andhakāratamonudo.",
  "tha-ap9:4.1_0": "Tesaṁ hāsaparetānaṁ,",
  "tha-ap9:4.2_0": "mahānādo avattatha;",
  "tha-ap9:4.3_0": "Kilese jhāpayissāma,",
  "tha-ap9:4.4_0": "sammāsambuddhasāsane.",
  "tha-ap9:5.1_0": "Devānaṁ giramaññāya,",
  "tha-ap9:5.2_0": "vācāsabhimudīrihaṁ;",
  "tha-ap9:5.3_0": "Haṭṭho haṭṭhena cittena,",
  "tha-ap9:5.4_0": "ādibhikkhamadāsahaṁ.",
  "tha-ap9:6.1_0": "Mama saṅkappamaññāya,",
  "tha-ap9:6.2_0": "satthā loke anuttaro;",
  "tha-ap9:6.3_0": "Devasaṅghe nisīditvā,",
  "tha-ap9:6.4_0": "imā gāthā abhāsatha.",
  "tha-ap9:7.1_0": "Sattāhaṁ abhinikkhamma,",
  "tha-ap9:7.2_0": "bodhiṁ ajjhagamaṁ ahaṁ;",
  "tha-ap9:7.3_0": "Idaṁ me paṭhamaṁ bhattaṁ,",
  "tha-ap9:7.4_0": "brahmacārissa yāpanaṁ.",
  "tha-ap9:8.1_0": "Tusitā hi idhāgantvā,",
  "tha-ap9:8.2_0": "yo me bhikkhaṁ upānayi;",
  "tha-ap9:8.3_0": "Tamahaṁ kittayissāmi,",
  "tha-ap9:8.4_0": "suṇotha mama bhāsato.",
  "tha-ap9:9.1_0": "Tiṁsakappasahassāni,",
  "tha-ap9:9.2_0": "devarajjaṁ karissati;",
  "tha-ap9:9.3_0": "Sabbe deve abhibhotvā,",
  "tha-ap9:9.4_0": "tidivaṁ āvasissati.",
  "tha-ap9:10.1_0": "Devalokā cavitvāna,",
  "tha-ap9:10.2_0": "manussattaṁ gamissati;",
  "tha-ap9:10.3_0": "Sahassadhā cakkavattī,",
  "tha-ap9:10.4_0": "tattha rajjaṁ karissati.",
  "tha-ap9:11.1_0": "Kappasatasahassamhi,",
  "tha-ap9:11.2_0": "okkākakulasambhavo;",
  "tha-ap9:11.3_0": "Gotamo nāma gottena,",
  "tha-ap9:11.4_0": "satthā loke bhavissati.",
  "tha-ap9:12.1_0": "Tidasā so cavitvāna,",
  "tha-ap9:12.2_0": "manussattaṁ gamissati;",
  "tha-ap9:12.3_0": "Agārā pabbajitvāna,",
  "tha-ap9:12.4_0": "chabbassāni vasissati.",
  "tha-ap9:13.1_0": "‘Tato sattamake vasse,",
  "tha-ap9:13.2_0": "buddho saccaṁ kathessati;",
  "tha-ap9:13.3_0": "Koṇḍañño nāma nāmena,",
  "tha-ap9:13.4_0": "paṭhamaṁ sacchikāhiti’.",
  "tha-ap9:14.1_0": "Nikkhantenānupabbajiṁ,",
  "tha-ap9:14.2_0": "padhānaṁ sukataṁ mayā;",
  "tha-ap9:14.3_0": "Kilese jhāpanatthāya,",
  "tha-ap9:14.4_0": "pabbajiṁ anagāriyaṁ.",
  "tha-ap9:15.1_0": "Abhigantvāna sabbaññū,",
  "tha-ap9:15.2_0": "buddho loke sadevake;",
  "tha-ap9:15.3_0": "Isināme migāraññe,",
  "tha-ap9:15.4_0": "amatabherimāhani.",
  "tha-ap9:16.1_0": "So dāni patto amataṁ,",
  "tha-ap9:16.2_0": "santipadamanuttaraṁ;",
  "tha-ap9:16.3_0": "Sabbāsave pariññāya,",
  "tha-ap9:16.4_0": "viharāmi anāsavo.",
  "tha-ap9:17.1_0": "Paṭisambhidā catasso,",
  "tha-ap9:17.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap9:17.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap9:17.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap9:18.1_0": "Itthaṁ sudaṁ āyasmā aññāsikoṇḍañño thero imā gāthāyo abhāsitthāti.",
  "tha-ap9:19.1_0": "Aññāsikoṇḍaññattherassāpadānaṁ sattamaṁ."
}