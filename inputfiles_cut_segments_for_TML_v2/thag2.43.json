{
  "thag2.43:0.1_0": "Theragāthā",
  "thag2.43:0.2_0": "Dukanipāta",
  "thag2.43:0.3_0": "Pañcamavagga",
  "thag2.43:0.4_0": "2.43. Brahmālittheragāthā",
  "thag2.43:1.1_0": "“Kassindriyāni samathaṅgatāni,",
  "thag2.43:1.2_0": "Assā yathā sārathinā sudantā;",
  "thag2.43:1.3_0": "Pahīnamānassa anāsavassa,",
  "thag2.43:1.4_0": "Devāpi kassa pihayanti tādinoti.",
  "thag2.43:2.1_0": "Mayhindriyāni samathaṅgatāni,",
  "thag2.43:2.2_0": "Assā yathā sārathinā sudantā;",
  "thag2.43:2.3_0": "Pahīnamānassa anāsavassa,",
  "thag2.43:2.4_0": "Devāpi mayhaṁ pihayanti tādino”ti.",
  "thag2.43:2.5_0": "… Brahmāli thero …."
}