{
  "ja323:0.1_0": "Jātaka",
  "ja323:0.2_0": "Catukkanipāta",
  "ja323:0.3_0": "Kuṭidūsakavagga",
  "ja323:0.4_0": "3. Brahmadattajātaka",
  "ja323:1.1_0": "“Dvayaṁ yācanako rāja,",
  "ja323:1.2_0": "brahmadatta nigacchati;",
  "ja323:1.3_0": "Alābhaṁ dhanalābhaṁ vā,",
  "ja323:1.4_0": "evaṁ dhammā hi yācanā.",
  "ja323:2.1_0": "Yācanaṁ rodanaṁ āhu,",
  "ja323:2.2_0": "pañcālānaṁ rathesabha;",
  "ja323:2.3_0": "Yo yācanaṁ paccakkhāti,",
  "ja323:2.4_0": "tamāhu paṭirodanaṁ.",
  "ja323:3.1_0": "Mā maddasaṁsu rodantaṁ,",
  "ja323:3.2_0": "pañcālā susamāgatā;",
  "ja323:3.3_0": "Tuvaṁ vā paṭirodantaṁ,",
  "ja323:3.4_0": "tasmā icchāmahaṁ raho”.",
  "ja323:4.1_0": "“Dadāmi te brāhmaṇa rohiṇīnaṁ,",
  "ja323:4.2_0": "Gavaṁ sahassaṁ saha puṅgavena;",
  "ja323:4.3_0": "Ariyo hi ariyassa kathaṁ na dajjā,",
  "ja323:4.4_0": "Sutvāna gāthā tava dhammayuttā”ti.",
  "ja323:5.1_0": "Brahmadattajātakaṁ tatiyaṁ."
}