{
  "ja374:0.1_0": "Jātaka",
  "ja374:0.2_0": "Pañcakanipāta",
  "ja374:0.3_0": "Aḍḍhavagga",
  "ja374:0.4_0": "4. Cūḷadhanuggahajātaka",
  "ja374:1.1_0": "“Sabbaṁ bhaṇḍaṁ samādāya,",
  "ja374:1.2_0": "pāraṁ tiṇṇosi brāhmaṇa;",
  "ja374:1.3_0": "Paccāgaccha lahuṁ khippaṁ,",
  "ja374:1.4_0": "mampi tārehi dānito”.",
  "ja374:2.1_0": "“Asanthutaṁ maṁ cirasanthutena,",
  "ja374:2.2_0": "Nimīni bhotī addhuvaṁ dhuvena;",
  "ja374:2.3_0": "Mayāpi bhotī nimineyya aññaṁ,",
  "ja374:2.4_0": "Ito ahaṁ dūrataraṁ gamissaṁ”.",
  "ja374:3.1_0": "“Kāyaṁ eḷagalāgumbe,",
  "ja374:3.2_0": "karoti ahuhāsiyaṁ;",
  "ja374:3.3_0": "Nayīdha naccagītaṁ vā,",
  "ja374:3.4_0": "tāḷaṁ vā susamāhitaṁ;",
  "ja374:3.5_0": "Anamhikāle susoṇi,",
  "ja374:3.6_0": "kiṁ nu jagghasi sobhane”.",
  "ja374:4.1_0": "“Siṅgāla bāla dummedha,",
  "ja374:4.2_0": "appapaññosi jambuka;",
  "ja374:4.3_0": "Jīno macchañca pesiñca,",
  "ja374:4.4_0": "kapaṇo viya jhāyasi”.",
  "ja374:5.1_0": "“Sudassaṁ vajjamaññesaṁ,",
  "ja374:5.2_0": "attano pana duddasaṁ;",
  "ja374:5.3_0": "Jīnā patiñca jārañca,",
  "ja374:5.4_0": "maññe tvaññeva jhāyasi”.",
  "ja374:6.1_0": "“Evametaṁ migarāja,",
  "ja374:6.2_0": "yathā bhāsasi jambuka;",
  "ja374:6.3_0": "Sā nūnāhaṁ ito gantvā,",
  "ja374:6.4_0": "bhattu hessaṁ vasānugā”.",
  "ja374:7.1_0": "“Yo hare mattikaṁ thālaṁ,",
  "ja374:7.2_0": "kaṁsathālampi so hare;",
  "ja374:7.3_0": "Katañceva tayā pāpaṁ,",
  "ja374:7.4_0": "punapevaṁ karissasī”ti.",
  "ja374:8.1_0": "Cūḷadhanuggahajātakaṁ catutthaṁ."
}