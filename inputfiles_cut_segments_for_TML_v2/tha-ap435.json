{
  "tha-ap435:0.1_0": "Therāpadāna",
  "tha-ap435:0.2_0": "Ekavihārivagga",
  "tha-ap435:0.3_0": "3 Pāṭihīrasaññakattheraapadāna",
  "tha-ap435:1.1_0": "“Padumuttaro nāma jino,",
  "tha-ap435:1.2_0": "āhutīnaṁ paṭiggaho;",
  "tha-ap435:1.3_0": "Vasīsatasahassehi,",
  "tha-ap435:1.4_0": "nagaraṁ pāvisī tadā.",
  "tha-ap435:2.1_0": "Nagaraṁ pavisantassa,",
  "tha-ap435:2.2_0": "upasantassa tādino;",
  "tha-ap435:2.3_0": "Ratanāni pajjotiṁsu,",
  "tha-ap435:2.4_0": "nigghoso āsi tāvade.",
  "tha-ap435:3.1_0": "Buddhassa ānubhāvena,",
  "tha-ap435:3.2_0": "bherī vajjumaghaṭṭitā;",
  "tha-ap435:3.3_0": "Sayaṁ vīṇā pavajjanti,",
  "tha-ap435:3.4_0": "buddhassa pavisato puraṁ.",
  "tha-ap435:4.1_0": "Buddhaseṭṭhaṁ namassāmi,",
  "tha-ap435:4.2_0": "padumuttaramahāmuniṁ;",
  "tha-ap435:4.3_0": "Pāṭihīrañca passitvā,",
  "tha-ap435:4.4_0": "tattha cittaṁ pasādayiṁ.",
  "tha-ap435:5.1_0": "Aho buddhā aho dhammā,",
  "tha-ap435:5.2_0": "aho no satthusampadā;",
  "tha-ap435:5.3_0": "Acetanāpi turiyā,",
  "tha-ap435:5.4_0": "sayameva pavajjare.",
  "tha-ap435:6.1_0": "Satasahassito kappe,",
  "tha-ap435:6.2_0": "yaṁ saññamalabhiṁ tadā;",
  "tha-ap435:6.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap435:6.4_0": "buddhasaññāyidaṁ phalaṁ.",
  "tha-ap435:7.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap435:7.2_0": "…pe…",
  "tha-ap435:7.3_0": "viharāmi anāsavo.",
  "tha-ap435:8.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap435:8.2_0": "…pe…",
  "tha-ap435:8.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap435:9.1_0": "Paṭisambhidā catasso,",
  "tha-ap435:9.2_0": "…pe…",
  "tha-ap435:9.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap435:10.1_0": "Itthaṁ sudaṁ āyasmā pāṭihīrasaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap435:11.1_0": "Pāṭihīrasaññakattherassāpadānaṁ tatiyaṁ."
}