{
  "tha-ap150:0.1_0": "Therāpadāna",
  "tha-ap150:0.2_0": "Chattavagga",
  "tha-ap150:0.3_0": "8. Phalakadāyakattheraapadāna",
  "tha-ap150:1.1_0": "“Yānakāro pure āsiṁ,",
  "tha-ap150:1.2_0": "dārukamme susikkhito;",
  "tha-ap150:1.3_0": "Candanaṁ phalakaṁ katvā,",
  "tha-ap150:1.4_0": "adāsiṁ lokabandhuno.",
  "tha-ap150:2.1_0": "Pabhāsati idaṁ byamhaṁ,",
  "tha-ap150:2.2_0": "suvaṇṇassa sunimmitaṁ;",
  "tha-ap150:2.3_0": "Hatthiyānaṁ assayānaṁ,",
  "tha-ap150:2.4_0": "dibbayānaṁ upaṭṭhitaṁ.",
  "tha-ap150:3.1_0": "Pāsādā sivikā ceva,",
  "tha-ap150:3.2_0": "Nibbattanti yadicchakaṁ;",
  "tha-ap150:3.3_0": "Akkhobhaṁ ratanaṁ mayhaṁ,",
  "tha-ap150:3.4_0": "Phalakassa idaṁ phalaṁ.",
  "tha-ap150:4.1_0": "Ekanavutito kappe,",
  "tha-ap150:4.2_0": "phalakaṁ yamahaṁ dadiṁ;",
  "tha-ap150:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap150:4.4_0": "phalakassa idaṁ phalaṁ.",
  "tha-ap150:5.1_0": "Sattapaññāsakappamhi,",
  "tha-ap150:5.2_0": "caturo nimmitāvhayā;",
  "tha-ap150:5.3_0": "Sattaratanasampannā,",
  "tha-ap150:5.4_0": "cakkavattī mahabbalā.",
  "tha-ap150:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap150:6.2_0": "…pe…",
  "tha-ap150:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap150:7.1_0": "Itthaṁ sudaṁ āyasmā phalakadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap150:8.1_0": "Phalakadāyakattherassāpadānaṁ aṭṭhamaṁ."
}