{
  "thag1.103:0.1_0": "Theragāthā",
  "thag1.103:0.2_0": "Ekakanipāta",
  "thag1.103:0.3_0": "Ekādasamavagga",
  "thag1.103:0.4_0": "1.103. Bandhurattheragāthā",
  "thag1.103:1.1_0": "“Nāhaṁ etena atthiko,",
  "thag1.103:1.2_0": "Sukhito dhammarasena tappito;",
  "thag1.103:1.3_0": "Pitvā rasaggamuttamaṁ,",
  "thag1.103:1.4_0": "Na ca kāhāmi visena santhavan”ti.",
  "thag1.103:1.5_0": "… Bandhuro thero …."
}