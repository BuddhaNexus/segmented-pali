{
  "vv63:0.1_0": "Vimānavatthu",
  "vv63:0.2_0": "Purisavimāna",
  "vv63:0.3_0": "Mahārathavagga",
  "vv63:0.4_0": "13. Cūḷarathavimānavatthu",
  "vv63:1.1_0": "“Daḷhadhammā nisārassa,",
  "vv63:1.2_0": "dhanuṁ olubbha tiṭṭhasi;",
  "vv63:1.3_0": "Khattiyo nusi rājañño,",
  "vv63:1.4_0": "adu luddo vanecaro”ti.",
  "vv63:2.1_0": "“Assakādhipatissāhaṁ,",
  "vv63:2.2_0": "bhante putto vanecaro;",
  "vv63:2.3_0": "Nāmaṁ me bhikkhu te brūmi,",
  "vv63:2.4_0": "sujāto iti maṁ vidū.",
  "vv63:3.1_0": "Mige gavesamānohaṁ,",
  "vv63:3.2_0": "Ogāhanto brahāvanaṁ;",
  "vv63:3.3_0": "Migaṁ tañceva nāddakkhiṁ,",
  "vv63:3.4_0": "Tañca disvā ṭhito ahan”ti.",
  "vv63:4.1_0": "“Svāgataṁ te mahāpuñña,",
  "vv63:4.2_0": "atho te adurāgataṁ;",
  "vv63:4.3_0": "Etto udakamādāya,",
  "vv63:4.4_0": "pāde pakkhālayassu te.",
  "vv63:5.1_0": "Idampi pānīyaṁ sītaṁ,",
  "vv63:5.2_0": "ābhataṁ girigabbharā;",
  "vv63:5.3_0": "Rājaputta tato pitvā,",
  "vv63:5.4_0": "santhatasmiṁ upāvisā”ti.",
  "vv63:6.1_0": "“Kalyāṇī vata te vācā,",
  "vv63:6.2_0": "Savanīyā mahāmuni;",
  "vv63:6.3_0": "Nelā atthavatī vaggu,",
  "vv63:6.4_0": "Mantvā atthañca bhāsasi.",
  "vv63:7.1_0": "Kā te rati vane viharato,",
  "vv63:7.2_0": "Isinisabha vadehi puṭṭho;",
  "vv63:7.3_0": "Tava vacanapathaṁ nisāmayitvā,",
  "vv63:7.4_0": "Atthadhammapadaṁ samācaremase”ti.",
  "vv63:8.1_0": "“Ahiṁsā sabbapāṇīnaṁ,",
  "vv63:8.2_0": "kumāramhāka ruccati;",
  "vv63:8.3_0": "Theyyā ca aticārā ca,",
  "vv63:8.4_0": "majjapānā ca ārati.",
  "vv63:9.1_0": "Ārati samacariyā ca,",
  "vv63:9.2_0": "bāhusaccaṁ kataññutā;",
  "vv63:9.3_0": "Diṭṭheva dhamme pāsaṁsā,",
  "vv63:9.4_0": "dhammā ete pasaṁsiyāti.",
  "vv63:10.1_0": "Santike maraṇaṁ tuyhaṁ,",
  "vv63:10.2_0": "oraṁ māsehi pañcahi;",
  "vv63:10.3_0": "Rājaputta vijānāhi,",
  "vv63:10.4_0": "attānaṁ parimocayā”ti.",
  "vv63:11.1_0": "“Katamaṁ svāhaṁ janapadaṁ gantvā,",
  "vv63:11.2_0": "Kiṁ kammaṁ kiñca porisaṁ;",
  "vv63:11.3_0": "Kāya vā pana vijjāya,",
  "vv63:11.4_0": "Bhaveyyaṁ ajarāmaro”ti.",
  "vv63:12.1_0": "“Na vijjate so padeso,",
  "vv63:12.2_0": "kammaṁ vijjā ca porisaṁ;",
  "vv63:12.3_0": "Yattha gantvā bhave macco,",
  "vv63:12.4_0": "rājaputtājarāmaro.",
  "vv63:13.1_0": "Mahaddhanā mahābhogā,",
  "vv63:13.2_0": "raṭṭhavantopi khattiyā;",
  "vv63:13.3_0": "Pahūtadhanadhaññāse,",
  "vv63:13.4_0": "tepi no ajarāmarā.",
  "vv63:14.1_0": "Yadi te sutā andhakaveṇḍuputtā,",
  "vv63:14.2_0": "Sūrā vīrā vikkantappahārino;",
  "vv63:14.3_0": "Tepi āyukkhayaṁ pattā,",
  "vv63:14.4_0": "Viddhastā sassatīsamā.",
  "vv63:15.1_0": "Khattiyā brāhmaṇā vessā,",
  "vv63:15.2_0": "suddā caṇḍālapukkusā;",
  "vv63:15.3_0": "Ete caññe ca jātiyā,",
  "vv63:15.4_0": "tepi no ajarāmarā.",
  "vv63:16.1_0": "Ye mantaṁ parivattenti,",
  "vv63:16.2_0": "chaḷaṅgaṁ brahmacintitaṁ;",
  "vv63:16.3_0": "Ete caññe ca vijjāya,",
  "vv63:16.4_0": "tepi no ajarāmarā.",
  "vv63:17.1_0": "Isayo cāpi ye santā,",
  "vv63:17.2_0": "saññatattā tapassino;",
  "vv63:17.3_0": "Sarīraṁ tepi kālena,",
  "vv63:17.4_0": "vijahanti tapassino.",
  "vv63:18.1_0": "Bhāvitattāpi arahanto,",
  "vv63:18.2_0": "katakiccā anāsavā;",
  "vv63:18.3_0": "Nikkhipanti imaṁ dehaṁ,",
  "vv63:18.4_0": "puññapāpaparikkhayā”ti.",
  "vv63:19.1_0": "“Subhāsitā atthavatī,",
  "vv63:19.2_0": "gāthāyo te mahāmuni;",
  "vv63:19.3_0": "Nijjhattomhi subhaṭṭhena,",
  "vv63:19.4_0": "tvañca me saraṇaṁ bhavā”ti.",
  "vv63:20.1_0": "“Mā maṁ tvaṁ saraṇaṁ gaccha,",
  "vv63:20.2_0": "tameva saraṇaṁ vaja;",
  "vv63:20.3_0": "Sakyaputtaṁ mahāvīraṁ,",
  "vv63:20.4_0": "yamahaṁ saraṇaṁ gato”ti.",
  "vv63:21.1_0": "“Katarasmiṁ so janapade,",
  "vv63:21.2_0": "satthā tumhāka mārisa;",
  "vv63:21.3_0": "Ahampi daṭṭhuṁ gacchissaṁ,",
  "vv63:21.4_0": "jinaṁ appaṭipuggalan”ti.",
  "vv63:22.1_0": "“Puratthimasmiṁ janapade,",
  "vv63:22.2_0": "okkākakulasambhavo;",
  "vv63:22.3_0": "Tatthāsi purisājañño,",
  "vv63:22.4_0": "so ca kho parinibbuto”ti.",
  "vv63:23.1_0": "“Sace hi buddho tiṭṭheyya,",
  "vv63:23.2_0": "Satthā tumhāka mārisa;",
  "vv63:23.3_0": "Yojanāni sahassāni,",
  "vv63:23.4_0": "Gaccheyyaṁ payirupāsituṁ.",
  "vv63:24.1_0": "Yato ca kho parinibbuto,",
  "vv63:24.2_0": "satthā tumhāka mārisa;",
  "vv63:24.3_0": "Nibbutampi mahāvīraṁ,",
  "vv63:24.4_0": "gacchāmi saraṇaṁ ahaṁ.",
  "vv63:25.1_0": "Upemi saraṇaṁ buddhaṁ,",
  "vv63:25.2_0": "dhammañcāpi anuttaraṁ;",
  "vv63:25.3_0": "Saṅghañca naradevassa,",
  "vv63:25.4_0": "gacchāmi saraṇaṁ ahaṁ.",
  "vv63:26.1_0": "Pāṇātipātā viramāmi khippaṁ,",
  "vv63:26.2_0": "Loke adinnaṁ parivajjayāmi;",
  "vv63:26.3_0": "Amajjapo no ca musā bhaṇāmi,",
  "vv63:26.4_0": "Sakena dārena ca homi tuṭṭho”ti.",
  "vv63:27.1_0": "“Sahassaraṁsīva yathā mahappabho,",
  "vv63:27.2_0": "Disaṁ yathā bhāti nabhe anukkamaṁ;",
  "vv63:27.3_0": "Tathāpakāro tavāyaṁ mahāratho,",
  "vv63:27.4_0": "Samantato yojanasattamāyato.",
  "vv63:28.1_0": "Suvaṇṇapaṭṭehi samantamotthaṭo,",
  "vv63:28.2_0": "Urassa muttāhi maṇīhi cittito;",
  "vv63:28.3_0": "Lekhā suvaṇṇassa ca rūpiyassa ca,",
  "vv63:28.4_0": "Sobhenti veḷuriyamayā sunimmitā.",
  "vv63:29.1_0": "Sīsañcidaṁ veḷuriyassa nimmitaṁ,",
  "vv63:29.2_0": "Yugañcidaṁ lohitakāya cittitaṁ;",
  "vv63:29.3_0": "Yuttā suvaṇṇassa ca rūpiyassa ca,",
  "vv63:29.4_0": "Sobhanti assā ca ime manojavā.",
  "vv63:30.1_0": "So tiṭṭhasi hemarathe adhiṭṭhito,",
  "vv63:30.2_0": "Devānamindova sahassavāhano;",
  "vv63:30.3_0": "Pucchāmi tāhaṁ yasavanta kovidaṁ,",
  "vv63:30.4_0": "Kathaṁ tayā laddho ayaṁ uḷāro”ti.",
  "vv63:31.1_0": "“Sujāto nāmahaṁ bhante,",
  "vv63:31.2_0": "rājaputto pure ahuṁ;",
  "vv63:31.3_0": "Tvañca maṁ anukampāya,",
  "vv63:31.4_0": "saññamasmiṁ nivesayi.",
  "vv63:32.1_0": "Khīṇāyukañca maṁ ñatvā,",
  "vv63:32.2_0": "sarīraṁ pādāsi satthuno;",
  "vv63:32.3_0": "Imaṁ sujāta pūjehi,",
  "vv63:32.4_0": "taṁ te atthāya hehiti.",
  "vv63:33.1_0": "Tāhaṁ gandhehi mālehi,",
  "vv63:33.2_0": "pūjayitvā samuyyuto;",
  "vv63:33.3_0": "Pahāya mānusaṁ dehaṁ,",
  "vv63:33.4_0": "upapannomhi nandanaṁ.",
  "vv63:34.1_0": "Nandane ca vane ramme,",
  "vv63:34.2_0": "nānādijagaṇāyute;",
  "vv63:34.3_0": "Ramāmi naccagītehi,",
  "vv63:34.4_0": "accharāhi purakkhato”ti.",
  "vv63:35.1_0": "Cūḷarathavimānaṁ terasamaṁ."
}