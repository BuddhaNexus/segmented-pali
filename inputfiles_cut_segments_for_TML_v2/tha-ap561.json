{
  "tha-ap561:0.1_0": "Therāpadāna",
  "tha-ap561:0.2_0": "Yasavagga",
  "tha-ap561:0.3_0": "9. Sivakattheraapadāna",
  "tha-ap561:1.1_0": "“Esanāya carantassa,",
  "tha-ap561:1.2_0": "vipassissa mahesino;",
  "tha-ap561:1.3_0": "Rittakaṁ pattaṁ disvāna,",
  "tha-ap561:1.4_0": "kummāsaṁ pūrayiṁ ahaṁ.",
  "tha-ap561:2.1_0": "Ekanavutito kappe,",
  "tha-ap561:2.2_0": "yaṁ bhikkhamadadiṁ tadā;",
  "tha-ap561:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap561:2.4_0": "kummāsassa idaṁ phalaṁ.",
  "tha-ap561:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap561:3.2_0": "…pe…",
  "tha-ap561:3.3_0": "viharāmi anāsavo.",
  "tha-ap561:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap561:4.2_0": "…pe…",
  "tha-ap561:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap561:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap561:5.2_0": "…pe…",
  "tha-ap561:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap561:6.1_0": "Itthaṁ sudaṁ āyasmā sivakatthero imā gāthāyo abhāsitthāti.",
  "tha-ap561:7.1_0": "Sivakattherassāpadānaṁ navamaṁ."
}