{
  "ja226:0.1_0": "Jātaka",
  "ja226:0.2_0": "Dukanipāta",
  "ja226:0.3_0": "Kāsāvavagga",
  "ja226:0.4_0": "6. Kosiyajātaka",
  "ja226:1.1_0": "“Kāle nikkhamanā sādhu,",
  "ja226:1.2_0": "nākāle sādhu nikkhamo;",
  "ja226:1.3_0": "Akālena hi nikkhamma,",
  "ja226:1.4_0": "ekakampi bahujjano;",
  "ja226:1.5_0": "Na kiñci atthaṁ joteti,",
  "ja226:1.6_0": "dhaṅkasenāva kosiyaṁ.",
  "ja226:2.1_0": "Dhīro ca vidhividhānaññū,",
  "ja226:2.2_0": "paresaṁ vivarānugū;",
  "ja226:2.3_0": "Sabbāmitte vasīkatvā,",
  "ja226:2.4_0": "kosiyova sukhī siyā”ti.",
  "ja226:3.1_0": "Kosiyajātakaṁ chaṭṭhaṁ."
}