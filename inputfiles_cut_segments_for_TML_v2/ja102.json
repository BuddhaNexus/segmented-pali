{
  "ja102:0.1_0": "Jātaka",
  "ja102:0.2_0": "Ekakanipāta",
  "ja102:0.3_0": "Parosatavagga",
  "ja102:0.4_0": "2. Paṇṇikajātaka",
  "ja102:1.1_0": "“Yo dukkhaphuṭṭhāya bhaveyya tāṇaṁ,",
  "ja102:1.2_0": "So me pitā dubbhi vane karoti;",
  "ja102:1.3_0": "Sā kassa kandāmi vanassa majjhe,",
  "ja102:1.4_0": "Yo tāyitā so sahasaṁ karotī”ti.",
  "ja102:2.1_0": "Paṇṇikajātakaṁ dutiyaṁ."
}