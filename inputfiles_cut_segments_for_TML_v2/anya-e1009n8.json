{
  "anya-e1009n8:0_0": "Gharāvāsa-niddesa",
  "anya-e1009n8:1_0": "171.",
  "anya-e1009n8:2_0": "Dukkhaṁ gahabbataṁ sādhu,",
  "anya-e1009n8:3_0": "Saṁvibhajjañca bhojanaṁ;",
  "anya-e1009n8:4_0": "A-hāso attha-lobhesu,",
  "anya-e1009n8:5_0": "Attha-byāpatti abyatho.",
  "anya-e1009n8:6_0": "172.",
  "anya-e1009n8:7_0": "Yodha sītañca uṇhañca,",
  "anya-e1009n8:8_0": "Tiṇā bhiyyo na maññati;",
  "anya-e1009n8:9_0": "Karaṁ purisa-kiccāni,",
  "anya-e1009n8:10_0": "So sukhā na vihāyati.",
  "anya-e1009n8:11_0": "173.",
  "anya-e1009n8:12_0": "Paṇḍito sīla-sampanno,",
  "anya-e1009n8:13_0": "Saṇhā ca paṭibhānavā;",
  "anya-e1009n8:14_0": "Nivāta-vutti atthaddho,",
  "anya-e1009n8:15_0": "Tādiso labhate yasaṁ.",
  "anya-e1009n8:16_0": "174.",
  "anya-e1009n8:17_0": "Uṭṭhānako analaso,",
  "anya-e1009n8:18_0": "Āpadāsu na vedhati;",
  "anya-e1009n8:19_0": "Acchinnavutti medhāvī,",
  "anya-e1009n8:20_0": "Tādiso labhate yasaṁ.",
  "anya-e1009n8:21_0": "175.",
  "anya-e1009n8:22_0": "Saṅgāhako mitta-karo,",
  "anya-e1009n8:23_0": "Vadaññū vītta-maccharo;",
  "anya-e1009n8:24_0": "Netā vi-netā anu-netā,",
  "anya-e1009n8:25_0": "Tādiso labhate yasaṁ.",
  "anya-e1009n8:26_0": "176.",
  "anya-e1009n8:27_0": "Uṭṭhānavako satīmato,",
  "anya-e1009n8:28_0": "Suci-kammassa nisammakārino;",
  "anya-e1009n8:29_0": "Saññatassa dhamma-jīvino,",
  "anya-e1009n8:30_0": "A-ppamattassa yasobhi-vaḍḍhati.",
  "anya-e1009n8:31_0": "177.",
  "anya-e1009n8:32_0": "Dveva tāta padākāni,",
  "anya-e1009n8:33_0": "Yattha sabbaṁ patiṭṭhitaṁ;",
  "anya-e1009n8:34_0": "A-laddhassa ca yo lābho,",
  "anya-e1009n8:35_0": "Laddhassa anurakkhaṇā.",
  "anya-e1009n8:36_0": "178.",
  "anya-e1009n8:37_0": "Catudhā vibhaje bhoge,",
  "anya-e1009n8:38_0": "Paṇḍito gharamāvasaṁ;",
  "anya-e1009n8:39_0": "Ekena bhogaṁ bhuñjeyya,",
  "anya-e1009n8:40_0": "Dvīhi kammaṁ payojaye;",
  "anya-e1009n8:41_0": "Catutthañca nidhāpeyya,",
  "anya-e1009n8:42_0": "Āpadāsu bhavissati.",
  "anya-e1009n8:43_0": "179.",
  "anya-e1009n8:44_0": "Añjanānaṁ khayaṁ disvā,",
  "anya-e1009n8:45_0": "Upacikānañca ācayaṁ;",
  "anya-e1009n8:46_0": "Madhūnañca samāhāraṁ,",
  "anya-e1009n8:47_0": "Paṇḍitto gharamāvase.",
  "anya-e1009n8:48_0": "180.",
  "anya-e1009n8:49_0": "Vibhavaṁ rakkhato laddhaṁ,",
  "anya-e1009n8:50_0": "Parihāni na vijjati;",
  "anya-e1009n8:51_0": "Ārakkhamhi a-santamhi,",
  "anya-e1009n8:52_0": "Laddhaṁ laddhaṁ vinassati.",
  "anya-e1009n8:53_0": "181.",
  "anya-e1009n8:54_0": "Paññā natthi dhanaṁ natthi,",
  "anya-e1009n8:55_0": "Yassa loke na vindati;",
  "anya-e1009n8:56_0": "Putta-dārā na pīyanti,",
  "anya-e1009n8:57_0": "Tassa mittaṁ sukhāvahaṁ.",
  "anya-e1009n8:58_0": "182.",
  "anya-e1009n8:59_0": "Cattāro ca veditabbā,",
  "anya-e1009n8:60_0": "Mittā ceva suhadā ca;",
  "anya-e1009n8:61_0": "Upakāro suhadopi,",
  "anya-e1009n8:62_0": "Samāna-sukha-dukkho ca;",
  "anya-e1009n8:63_0": "Atthakkhāyīnukampako,",
  "anya-e1009n8:64_0": "Tathā mitto veditabbo.",
  "anya-e1009n8:65_0": "183.",
  "anya-e1009n8:66_0": "Bhogā naṭṭhena jiṇṇena,",
  "anya-e1009n8:67_0": "A-mitena ca bhojane;",
  "anya-e1009n8:68_0": "Na tiṭṭhanti ciraṁ disvā,",
  "anya-e1009n8:69_0": "Taṁ paṇḍito ghare vase.",
  "anya-e1009n8:70_0": "184.",
  "anya-e1009n8:71_0": "Ati-sītaṁ ati-uṇhaṁ,",
  "anya-e1009n8:72_0": "Ati-sāyamidaṁ ahu;",
  "anya-e1009n8:73_0": "Iti vissaṭṭha-kammantte,",
  "anya-e1009n8:74_0": "Atthā accenti māṇave.",
  "anya-e1009n8:75_0": "185.",
  "anya-e1009n8:76_0": "Na divā suppa-sīlena,",
  "anya-e1009n8:77_0": "Rattinaṭṭhānadessinā;",
  "anya-e1009n8:78_0": "Niccaṁ mattena soṇḍena,",
  "anya-e1009n8:79_0": "Sakkā āvasituṁ gharaṁ.",
  "anya-e1009n8:80_0": "186.",
  "anya-e1009n8:81_0": "Hananti bhogā dummedhaṁ,",
  "anya-e1009n8:82_0": "No ce pāra-gavesino;",
  "anya-e1009n8:83_0": "Bhoga-taṇhāya dummedho,",
  "anya-e1009n8:84_0": "Hanti aññeva attanaṁ.",
  "anya-e1009n8:85_0": "187.",
  "anya-e1009n8:86_0": "Dujjīvitamajīvimhā,",
  "anya-e1009n8:87_0": "Yesaṁ no na dadāmase;",
  "anya-e1009n8:88_0": "Vijjamānesu bhogesu,",
  "anya-e1009n8:89_0": "Dīpaṁ nā kamha mattano.",
  "anya-e1009n8:90_0": "188.",
  "anya-e1009n8:91_0": "Saṭṭhi-vassa-sahassāni,",
  "anya-e1009n8:92_0": "Paripuṇṇāni sabbaso;",
  "anya-e1009n8:93_0": "Niraye paccamānānaṁ,",
  "anya-e1009n8:94_0": "Kadā anto bhavissati.",
  "anya-e1009n8:95_0": "189.",
  "anya-e1009n8:96_0": "Natthi anto kuto anto,",
  "anya-e1009n8:97_0": "Na anto patidissati;",
  "anya-e1009n8:98_0": "Tadā hi pakataṁ pāpaṁ,",
  "anya-e1009n8:99_0": "Mama tuyhañce mārisā;",
  "anya-e1009n8:100_0": "190.",
  "anya-e1009n8:101_0": "Sohaṁ nūna ito gantā,",
  "anya-e1009n8:102_0": "Yoni laddhāna mānusaṁ;",
  "anya-e1009n8:103_0": "Vadaññū sīla-sampanno,",
  "anya-e1009n8:104_0": "Kāhāmi kusalaṁ bahuṁ.",
  "anya-e1009n8:105_0": "191.",
  "anya-e1009n8:106_0": "Mā gijjhe paccaye macco,",
  "anya-e1009n8:107_0": "Bahu-dosā hi paccayā;",
  "anya-e1009n8:108_0": "Caranto paccaye ñāyā,",
  "anya-e1009n8:109_0": "Ubhayatthāpi vaḍḍhati.",
  "anya-e1009n8:110_0": "192.",
  "anya-e1009n8:111_0": "A-laddhā vittaṁ tappati,",
  "anya-e1009n8:112_0": "Pubbe a-samudānitaṁ;",
  "anya-e1009n8:113_0": "Na pubbe dhanamesissaṁ,",
  "anya-e1009n8:114_0": "Iti pacchānutappati.",
  "anya-e1009n8:115_0": "193.",
  "anya-e1009n8:116_0": "Kūṭavedī pure āsiṁ,",
  "anya-e1009n8:117_0": "Pisuṇo piṭṭhi-maṁsiko;",
  "anya-e1009n8:118_0": "Caṇḍo ca pharuso cāpi,",
  "anya-e1009n8:119_0": "Iti pacchānutappati.",
  "anya-e1009n8:120_0": "194.",
  "anya-e1009n8:121_0": "Pāṇātipātī pure āsiṁ,",
  "anya-e1009n8:122_0": "Luddo cāpi anariyo;",
  "anya-e1009n8:123_0": "Bhūtānaṁ nānukampiyaṁ,",
  "anya-e1009n8:124_0": "Iti pacchānutappati.",
  "anya-e1009n8:125_0": "195.",
  "anya-e1009n8:126_0": "Bahūsu vata santīsu,",
  "anya-e1009n8:127_0": "Anāpādāsu itthisu;",
  "anya-e1009n8:128_0": "Para-dāraṁ asevissaṁ,",
  "anya-e1009n8:129_0": "Iti pacchānutappati.",
  "anya-e1009n8:130_0": "196.",
  "anya-e1009n8:131_0": "Bahumhi tava santamhi,",
  "anya-e1009n8:132_0": "Anna-pāne upaṭṭhite;",
  "anya-e1009n8:133_0": "Na pubbe adadaṁ dānaṁ,",
  "anya-e1009n8:134_0": "Iti pacchānutappati.",
  "anya-e1009n8:135_0": "197.",
  "anya-e1009n8:136_0": "Mātaraṁ pitarañcāpi,",
  "anya-e1009n8:137_0": "Jiṇṇakaṁ gata-yobbanaṁ;",
  "anya-e1009n8:138_0": "Pahu santo na posissaṁ,",
  "anya-e1009n8:139_0": "Iti pacchānukappati.",
  "anya-e1009n8:140_0": "198.",
  "anya-e1009n8:141_0": "Ācariyamanusatthāraṁ,",
  "anya-e1009n8:142_0": "Sabba-kāma-rasāharaṁ;",
  "anya-e1009n8:143_0": "Pitaraṁ atimaññissaṁ,",
  "anya-e1009n8:144_0": "Iti pacchānutappati.",
  "anya-e1009n8:145_0": "199.",
  "anya-e1009n8:146_0": "Samaṇe brāhmaṇe cāpi,",
  "anya-e1009n8:147_0": "Sīlavante bahussute;",
  "anya-e1009n8:148_0": "Na pubbe payirupāsissaṁ,",
  "anya-e1009n8:149_0": "Iti pacchānutappati.",
  "anya-e1009n8:150_0": "200.",
  "anya-e1009n8:151_0": "Sādhu hoti tapo ciṇṇo,",
  "anya-e1009n8:152_0": "Santo ca payirupāsito;",
  "anya-e1009n8:153_0": "Na pubbeva tapociṇṇo,",
  "anya-e1009n8:154_0": "Iti pacchānutappati.",
  "anya-e1009n8:155_0": "201.",
  "anya-e1009n8:156_0": "Yo ca etāni ṭhānāni,",
  "anya-e1009n8:157_0": "Yoniso paṭipajjati;",
  "anya-e1009n8:158_0": "Karaṁ purisa-kiccāni,",
  "anya-e1009n8:159_0": "Sa pacchā nānutappati.",
  "anya-e1009n8:160_0": "202.",
  "anya-e1009n8:161_0": "Na sādhāraṇa-dārassa,",
  "anya-e1009n8:162_0": "Na bhuñje sādhumekako;",
  "anya-e1009n8:163_0": "Na seve lokāyatikaṁ,",
  "anya-e1009n8:164_0": "Netaṁ paññāya vaḍḍhanaṁ.",
  "anya-e1009n8:165_0": "203.",
  "anya-e1009n8:166_0": "Sīlavā vatta-sampanno,",
  "anya-e1009n8:167_0": "A-ppamatto vicakkhaṇo;",
  "anya-e1009n8:168_0": "Nivātta-vutti atthaddho,",
  "anya-e1009n8:169_0": "Surato sakhito mudu.",
  "anya-e1009n8:170_0": "204.",
  "anya-e1009n8:171_0": "Saṅgahetā ca mittānaṁ,",
  "anya-e1009n8:172_0": "Saṁvibhāgī vīdhānavā;",
  "anya-e1009n8:173_0": "Tappeyya anna-pānena,",
  "anya-e1009n8:174_0": "Sadā samaṇa-brāhmaṇe.",
  "anya-e1009n8:175_0": "205.",
  "anya-e1009n8:176_0": "Dhamma-kāmo sutā-dhāro,",
  "anya-e1009n8:177_0": "Bhaveyya paripucchako;",
  "anya-e1009n8:178_0": "Sakkaccaṁ payirupāseyya,",
  "anya-e1009n8:179_0": "Sīlavante bahussutte.",
  "anya-e1009n8:180_0": "206.",
  "anya-e1009n8:181_0": "Gharamāvasamānassa,",
  "anya-e1009n8:182_0": "Gahaṭṭhassa sakaṁ gharaṁ;",
  "anya-e1009n8:183_0": "Khemā vutti siyā evaṁ,",
  "anya-e1009n8:184_0": "Evaṁ nu assa saṅgaho.",
  "anya-e1009n8:185_0": "207.",
  "anya-e1009n8:186_0": "A-byapajjo siyā evaṁ,",
  "anya-e1009n8:187_0": "Sacca-vādī ca māṇavo;",
  "anya-e1009n8:188_0": "Asmā lokā paraṁ lokaṁ,",
  "anya-e1009n8:189_0": "Evaṁ pecca na socati.",
  "anya-e1009n8:190_0": "208.",
  "anya-e1009n8:191_0": "Sukhā matteyyatā loke,",
  "anya-e1009n8:192_0": "Atho petteyyatā sukhā;",
  "anya-e1009n8:193_0": "Sukhā sāmaññatā loke,",
  "anya-e1009n8:194_0": "Atho brahmaññatā sukhā.",
  "anya-e1009n8:195_0": "209.",
  "anya-e1009n8:196_0": "Pathavī veḷukaṁ pattaṁ,",
  "anya-e1009n8:197_0": "Cakkavāḷaṁ sucipphalaṁ;",
  "anya-e1009n8:198_0": "Sineru vammiko khuddo,",
  "anya-e1009n8:199_0": "Samuddo pātiko yathā.",
  "anya-e1009n8:200_0": "210.",
  "anya-e1009n8:201_0": "Brahmāti mātā-pitaro,",
  "anya-e1009n8:202_0": "Pubbācariyāti vuccare;",
  "anya-e1009n8:203_0": "Āhuneyyā ca puttānaṁ,",
  "anya-e1009n8:204_0": "Pajānamanukampakā.",
  "anya-e1009n8:205_0": "211.",
  "anya-e1009n8:206_0": "Tasmā hi ne namaseyya,",
  "anya-e1009n8:207_0": "Sakkareyya ca paṇḍito;",
  "anya-e1009n8:208_0": "Annena atho pānena,",
  "anya-e1009n8:209_0": "Vatthena sayanena ca."
}