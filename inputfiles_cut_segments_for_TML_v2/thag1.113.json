{
  "thag1.113:0.1_0": "Theragāthā",
  "thag1.113:0.2_0": "Ekakanipāta",
  "thag1.113:0.3_0": "Dvādasamavagga",
  "thag1.113:0.4_0": "1.113. Vanavacchattheragāthā",
  "thag1.113:1.1_0": "“Acchodikā puthusilā,",
  "thag1.113:1.2_0": "gonaṅgulamigāyutā;",
  "thag1.113:1.3_0": "Ambusevālasañchannā,",
  "thag1.113:1.4_0": "te selā ramayanti man”ti.",
  "thag1.113:1.5_0": "… Vanavaccho thero …."
}