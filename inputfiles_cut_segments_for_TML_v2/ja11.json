{
  "ja11:0.1_0": "Jātaka",
  "ja11:0.2_0": "Ekakanipāta",
  "ja11:0.3_0": "Sīlavagga",
  "ja11:0.4_0": "1. Lakkhaṇamigajātaka",
  "ja11:1.1_0": "“Hoti sīlavataṁ attho,",
  "ja11:1.2_0": "paṭisanthāravuttinaṁ;",
  "ja11:1.3_0": "Lakkhaṇaṁ passa āyantaṁ,",
  "ja11:1.4_0": "ñātisaṅghapurakkhataṁ;",
  "ja11:1.5_0": "Atha passasimaṁ kāḷaṁ,",
  "ja11:1.6_0": "suvihīnaṁva ñātibhī”ti.",
  "ja11:2.1_0": "Lakkhaṇamigajātakaṁ paṭhamaṁ."
}