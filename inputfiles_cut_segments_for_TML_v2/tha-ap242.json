{
  "tha-ap242:0.1_0": "Therāpadāna",
  "tha-ap242:0.2_0": "Udakāsanavagga",
  "tha-ap242:0.3_0": "10. Vāsidāyakattheraapadāna",
  "tha-ap242:1.1_0": "“Kammārohaṁ pure āsiṁ,",
  "tha-ap242:1.2_0": "tivarāyaṁ puruttame;",
  "tha-ap242:1.3_0": "Ekā vāsi mayā dinnā,",
  "tha-ap242:1.4_0": "sayambhuṁ aparājitaṁ.",
  "tha-ap242:2.1_0": "Catunnavutito kappe,",
  "tha-ap242:2.2_0": "yaṁ vāsimadadiṁ tadā;",
  "tha-ap242:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap242:2.4_0": "vāsidānassidaṁ phalaṁ.",
  "tha-ap242:3.1_0": "Paṭisambhidā catasso,",
  "tha-ap242:3.2_0": "…pe…",
  "tha-ap242:3.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap242:4.1_0": "Itthaṁ sudaṁ āyasmā vāsidāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap242:5.1_0": "Vāsidāyakattherassāpadānaṁ dasamaṁ.",
  "tha-ap242:6.1_0": "Udakāsanavaggo catuvīsatimo.",
  "tha-ap242:7.1_0": "Tassuddānaṁ",
  "tha-ap242:8.1_0": "Udakāsanabhājanaṁ,",
  "tha-ap242:8.2_0": "sālapupphī kilañjako;",
  "tha-ap242:8.3_0": "Vedikā vaṇṇakāro ca,",
  "tha-ap242:8.4_0": "piyālaambayāgado;",
  "tha-ap242:8.5_0": "Jagatī vāsidātā ca,",
  "tha-ap242:8.6_0": "gāthā tiṁsa ca aṭṭha ca."
}