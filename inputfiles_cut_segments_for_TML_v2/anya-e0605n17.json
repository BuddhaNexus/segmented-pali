{
  "anya-e0605n17:0_0": "Ekaṭhānikādiyamakagāthā",
  "anya-e0605n17:1_0": "101.",
  "anya-e0605n17:2_0": "Ākaṅkhakkhākaṅkhaṅga kaṅkhāgaṅgākhāgahaka,",
  "anya-e0605n17:3_0": "Kaṅkhāgāhakakaṅkhāgha hā hā kaṅkhā kahaṁ kahaṁ.",
  "anya-e0605n17:4_0": "102.",
  "anya-e0605n17:5_0": "Appagabbho apagabbho amoho mā pamohako,",
  "anya-e0605n17:6_0": "Maggamukhaṁ mokhamāha māhā mohamūhakkhamaṁ.",
  "anya-e0605n17:7_0": "103.",
  "anya-e0605n17:8_0": "Pāpāpāpabhavaṁ passaṁ pāpāpabhavuggato,",
  "anya-e0605n17:9_0": "Pāpāpāpabhavāsaṅgā pāpāpāpabhavāgato.",
  "anya-e0605n17:10_0": "104.",
  "anya-e0605n17:11_0": "Kusalākusalaṁ passaṁ kusalākusalaṁ caji,",
  "anya-e0605n17:12_0": "Kusalākusalāsaṅga kusalākusalā cuto."
}