{
  "ja306:0.1_0": "Jātaka",
  "ja306:0.2_0": "Catukkanipāta",
  "ja306:0.3_0": "Kāliṅgavagga",
  "ja306:0.4_0": "6. Sujātājātaka",
  "ja306:1.1_0": "“Kimaṇḍakā ime deva,",
  "ja306:1.2_0": "nikkhittā kaṁsamallake;",
  "ja306:1.3_0": "Upalohitakā vaggū,",
  "ja306:1.4_0": "taṁ me akkhāhi pucchito”.",
  "ja306:2.1_0": "“Yāni pure tuvaṁ devi,",
  "ja306:2.2_0": "bhaṇḍu nantakavāsinī;",
  "ja306:2.3_0": "Ucchaṅgahatthā pacināsi,",
  "ja306:2.4_0": "tassā te koliyaṁ phalaṁ.",
  "ja306:3.1_0": "Uḍḍayhate na ramati,",
  "ja306:3.2_0": "bhogā vippajahanti taṁ;",
  "ja306:3.3_0": "Tatthevimaṁ paṭinetha,",
  "ja306:3.4_0": "yattha kolaṁ pacissati”.",
  "ja306:4.1_0": "“Honti hete mahārāja,",
  "ja306:4.2_0": "iddhippattāya nāriyā;",
  "ja306:4.3_0": "Khama deva sujātāya,",
  "ja306:4.4_0": "māssā kujjha rathesabhā”ti.",
  "ja306:5.1_0": "Sujātājātakaṁ chaṭṭhaṁ."
}