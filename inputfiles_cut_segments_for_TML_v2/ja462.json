{
  "ja462:0.1_0": "Jātaka",
  "ja462:0.2_0": "Ekādasakanipāta",
  "ja462:0.3_0": "Mātuposakavagga",
  "ja462:0.4_0": "8. Saṁvarajātaka",
  "ja462:1.1_0": "“Jānanto no mahārāja,",
  "ja462:1.2_0": "tava sīlaṁ janādhipo;",
  "ja462:1.3_0": "Ime kumāre pūjento,",
  "ja462:1.4_0": "na taṁ kenaci maññatha.",
  "ja462:2.1_0": "Tiṭṭhante no mahārāje,",
  "ja462:2.2_0": "adu deve divaṅgate;",
  "ja462:2.3_0": "Ñātī taṁ samanuññiṁsu,",
  "ja462:2.4_0": "sampassaṁ atthamattano.",
  "ja462:3.1_0": "Kena saṁvara vattena,",
  "ja462:3.2_0": "sañjāte abhitiṭṭhasi;",
  "ja462:3.3_0": "Kena taṁ nātivattanti,",
  "ja462:3.4_0": "ñātisaṅghā samāgatā”.",
  "ja462:4.1_0": "“Na rājaputta usūyāmi,",
  "ja462:4.2_0": "samaṇānaṁ mahesinaṁ;",
  "ja462:4.3_0": "Sakkaccaṁ te namassāmi,",
  "ja462:4.4_0": "pāde vandāmi tādinaṁ.",
  "ja462:5.1_0": "Te maṁ dhammaguṇe yuttaṁ,",
  "ja462:5.2_0": "sussūsamanusūyakaṁ;",
  "ja462:5.3_0": "Samaṇā manusāsanti,",
  "ja462:5.4_0": "isī dhammaguṇe ratā.",
  "ja462:6.1_0": "Tesāhaṁ vacanaṁ sutvā,",
  "ja462:6.2_0": "samaṇānaṁ mahesinaṁ;",
  "ja462:6.3_0": "Na kiñci atimaññāmi,",
  "ja462:6.4_0": "dhamme me nirato mano.",
  "ja462:7.1_0": "Hatthārohā anīkaṭṭhā,",
  "ja462:7.2_0": "rathikā pattikārakā;",
  "ja462:7.3_0": "Tesaṁ nappaṭibandhāmi,",
  "ja462:7.4_0": "niviṭṭhaṁ bhattavetanaṁ.",
  "ja462:8.1_0": "Mahāmattā ca me atthi,",
  "ja462:8.2_0": "mantino paricārakā;",
  "ja462:8.3_0": "Bārāṇasiṁ voharanti,",
  "ja462:8.4_0": "bahumaṁsasurodakaṁ.",
  "ja462:9.1_0": "Athopi vāṇijā phītā,",
  "ja462:9.2_0": "nānāraṭṭhehi āgatā;",
  "ja462:9.3_0": "Tesu me vihitā rakkhā,",
  "ja462:9.4_0": "evaṁ jānāhuposatha”.",
  "ja462:10.1_0": "“Dhammena kira ñātīnaṁ,",
  "ja462:10.2_0": "rajjaṁ kārehi saṁvara;",
  "ja462:10.3_0": "Medhāvī paṇḍito cāsi,",
  "ja462:10.4_0": "athopi ñātinaṁ hito.",
  "ja462:11.1_0": "Taṁ taṁ ñātiparibyūḷhaṁ,",
  "ja462:11.2_0": "nānāratanamocitaṁ;",
  "ja462:11.3_0": "Amittā nappasahanti,",
  "ja462:11.4_0": "indaṁva asurādhipo”ti.",
  "ja462:12.1_0": "Saṁvarajātakaṁ aṭṭhamaṁ."
}