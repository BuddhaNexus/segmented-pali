{
  "ja441:0.1_0": "Jātaka",
  "ja441:0.2_0": "Dasakanipāta",
  "ja441:0.3_0": "Catudvāravagga",
  "ja441:0.4_0": "3. Catuposathiyajātaka",
  "ja441:1.1_0": "“Yo kopaneyye na karoti kopaṁ,",
  "ja441:1.2_0": "Na kujjhati sappuriso kadāci;",
  "ja441:1.3_0": "Kuddhopi so nāvikaroti kopaṁ,",
  "ja441:1.4_0": "Taṁ ve naraṁ samaṇamāhu loke”.",
  "ja441:2.1_0": "“Ūnūdaro yo sahate jighacchaṁ,",
  "ja441:2.2_0": "Danto tapassī mitapānabhojano;",
  "ja441:2.3_0": "Āhārahetu na karoti pāpaṁ,",
  "ja441:2.4_0": "Taṁ ve naraṁ samaṇamāhu loke”.",
  "ja441:3.1_0": "“Khiḍḍaṁ ratiṁ vippajahitvāna sabbaṁ,",
  "ja441:3.2_0": "Na cālikaṁ bhāsasi kiñci loke;",
  "ja441:3.3_0": "Vibhūsaṭṭhānā virato methunasmā,",
  "ja441:3.4_0": "Taṁ ve naraṁ samaṇamāhu loke”.",
  "ja441:4.1_0": "“Pariggahaṁ lobhadhammañca sabbaṁ,",
  "ja441:4.2_0": "Yo ve pariññāya pariccajeti;",
  "ja441:4.3_0": "Dantaṁ ṭhitattaṁ amamaṁ nirāsaṁ,",
  "ja441:4.4_0": "Taṁ ve naraṁ samaṇamāhu loke”.",
  "ja441:5.1_0": "“Pucchāma kattāramanomapaññaṁ,",
  "ja441:5.2_0": "Kathāsu no viggaho atthi jāto;",
  "ja441:5.3_0": "Chindajja kaṅkhaṁ vicikicchitāni,",
  "ja441:5.4_0": "Tadajja kaṅkhaṁ vitaremu sabbe”.",
  "ja441:6.1_0": "“Ye paṇḍitā atthadasā bhavanti,",
  "ja441:6.2_0": "Bhāsanti te yoniso tattha kāle;",
  "ja441:6.3_0": "Kathaṁ nu kathānaṁ abhāsitānaṁ,",
  "ja441:6.4_0": "Atthaṁ nayeyyuṁ kusalā janindā.",
  "ja441:7.1_0": "Kathaṁ have bhāsati nāgarājā,",
  "ja441:7.2_0": "Garuḷo pana venateyyo kimāha;",
  "ja441:7.3_0": "Gandhabbarājā pana kiṁ vadesi,",
  "ja441:7.4_0": "Kathaṁ pana kurūnaṁ rājaseṭṭho”.",
  "ja441:8.1_0": "“Khantiṁ have bhāsati nāgarājā,",
  "ja441:8.2_0": "Appāhāraṁ garuḷo venateyyo;",
  "ja441:8.3_0": "Gandhabbarājā rativippahānaṁ,",
  "ja441:8.4_0": "Akiñcanaṁ kurūnaṁ rājaseṭṭho”.",
  "ja441:9.1_0": "“Sabbāni etāni subhāsitāni,",
  "ja441:9.2_0": "Na hettha dubbhāsitamatthi kiñci;",
  "ja441:9.3_0": "Yasmiñca etāni patiṭṭhitāni,",
  "ja441:9.4_0": "Arāva nābhyā susamohitāni;",
  "ja441:9.5_0": "Catubbhi dhammehi samaṅgibhūtaṁ,",
  "ja441:9.6_0": "Taṁ ve naraṁ samaṇamāhu loke”.",
  "ja441:10.1_0": "“Tuvañhi seṭṭho tvamanuttarosi,",
  "ja441:10.2_0": "Tvaṁ dhammagū dhammavidū sumedho;",
  "ja441:10.3_0": "Paññāya pañhaṁ samadhiggahetvā,",
  "ja441:10.4_0": "Acchecchi dhīro vicikicchitāni;",
  "ja441:10.5_0": "Acchecchi kaṅkhaṁ vicikicchitāni,",
  "ja441:10.6_0": "Cundo yathā nāgadantaṁ kharena.",
  "ja441:11.1_0": "Nīluppalābhaṁ vimalaṁ anagghaṁ,",
  "ja441:11.2_0": "Vatthaṁ idaṁ dhūmasamānavaṇṇaṁ;",
  "ja441:11.3_0": "Pañhassa veyyākaraṇena tuṭṭho,",
  "ja441:11.4_0": "Dadāmi te dhammapūjāya dhīra.",
  "ja441:12.1_0": "Suvaṇṇamālaṁ satapattaphullitaṁ,",
  "ja441:12.2_0": "Sakesaraṁ ratnasahassamaṇḍitaṁ;",
  "ja441:12.3_0": "Pañhassa veyyākaraṇena tuṭṭho,",
  "ja441:12.4_0": "Dadāmi te dhammapūjāya dhīra.",
  "ja441:13.1_0": "Maṇiṁ anagghaṁ ruciraṁ pabhassaraṁ,",
  "ja441:13.2_0": "Kaṇṭhāvasattaṁ maṇibhūsitaṁ me;",
  "ja441:13.3_0": "Pañhassa veyyākaraṇena tuṭṭho,",
  "ja441:13.4_0": "Dadāmi te dhammapūjāya dhīra.",
  "ja441:14.1_0": "Gavaṁ sahassaṁ usabhañca nāgaṁ,",
  "ja441:14.2_0": "Ājaññayutte ca rathe dasa ime;",
  "ja441:14.3_0": "Pañhassa veyyākaraṇena tuṭṭho,",
  "ja441:14.4_0": "Dadāmi te gāmavarāni soḷasa”.",
  "ja441:15.1_0": "“Sāriputto tadā nāgo,",
  "ja441:15.2_0": "supaṇṇo pana kolito;",
  "ja441:15.3_0": "Gandhabbarājā anuruddho,",
  "ja441:15.4_0": "rājā ānandapaṇḍito;",
  "ja441:15.5_0": "Vidhuro bodhisatto ca,",
  "ja441:15.6_0": "evaṁ dhāretha jātakan”ti.",
  "ja441:16.1_0": "Catuposathiyajātakaṁ tatiyaṁ."
}