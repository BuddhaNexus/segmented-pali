{
  "thag3.4:0.1_0": "Theragāthā",
  "thag3.4:0.2_0": "Tikanipāta",
  "thag3.4:0.3_0": "Paṭhamavagga",
  "thag3.4:0.4_0": "3.4. Dhaniyattheragāthā",
  "thag3.4:1.1_0": "“Sukhañce jīvituṁ icche,",
  "thag3.4:1.2_0": "sāmaññasmiṁ apekkhavā;",
  "thag3.4:1.3_0": "Saṅghikaṁ nātimaññeyya,",
  "thag3.4:1.4_0": "cīvaraṁ pānabhojanaṁ.",
  "thag3.4:2.1_0": "Sukhañce jīvituṁ icche,",
  "thag3.4:2.2_0": "sāmaññasmiṁ apekkhavā;",
  "thag3.4:2.3_0": "Ahi mūsikasobbhaṁva,",
  "thag3.4:2.4_0": "sevetha sayanāsanaṁ.",
  "thag3.4:3.1_0": "Sukhañce jīvituṁ icche,",
  "thag3.4:3.2_0": "sāmaññasmiṁ apekkhavā;",
  "thag3.4:3.3_0": "Itarītarena tusseyya,",
  "thag3.4:3.4_0": "ekadhammañca bhāvaye”ti.",
  "thag3.4:3.5_0": "… Dhaniyo thero …."
}