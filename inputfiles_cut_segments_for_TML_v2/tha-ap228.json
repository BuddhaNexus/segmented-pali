{
  "tha-ap228:0.1_0": "Therāpadāna",
  "tha-ap228:0.2_0": "Ālambaṇadāyakavagga",
  "tha-ap228:0.3_0": "6 Aṅkolapupphiyattheraapadāna",
  "tha-ap228:1.1_0": "“Nārado iti me nāmaṁ,",
  "tha-ap228:1.2_0": "kassapo iti maṁ vidū;",
  "tha-ap228:1.3_0": "Addasaṁ samaṇānaggaṁ,",
  "tha-ap228:1.4_0": "vipassiṁ devasakkataṁ.",
  "tha-ap228:2.1_0": "Anubyañjanadharaṁ buddhaṁ,",
  "tha-ap228:2.2_0": "āhutīnaṁ paṭiggahaṁ;",
  "tha-ap228:2.3_0": "Aṅkolapupphaṁ paggayha,",
  "tha-ap228:2.4_0": "buddhassa abhiropayiṁ.",
  "tha-ap228:3.1_0": "Ekanavutito kappe,",
  "tha-ap228:3.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap228:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap228:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap228:4.1_0": "Catusattatito kappe,",
  "tha-ap228:4.2_0": "romaso nāma khattiyo;",
  "tha-ap228:4.3_0": "Āmukkamālābharaṇo,",
  "tha-ap228:4.4_0": "sayoggabalavāhano.",
  "tha-ap228:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap228:5.2_0": "…pe…",
  "tha-ap228:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap228:6.1_0": "Itthaṁ sudaṁ āyasmā aṅkolapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap228:7.1_0": "Aṅkolapupphiyattherassāpadānaṁ chaṭṭhaṁ."
}