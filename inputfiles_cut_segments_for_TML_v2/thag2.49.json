{
  "thag2.49:0.1_0": "Theragāthā",
  "thag2.49:0.2_0": "Dukanipāta",
  "thag2.49:0.3_0": "Pañcamavagga",
  "thag2.49:0.4_0": "2.49. Sandhitattheragāthā",
  "thag2.49:1.1_0": "“Assatthe haritobhāse,",
  "thag2.49:1.2_0": "saṁvirūḷhamhi pādape;",
  "thag2.49:1.3_0": "Ekaṁ buddhagataṁ saññaṁ,",
  "thag2.49:1.4_0": "alabhitthaṁ patissato.",
  "thag2.49:2.1_0": "Ekatiṁse ito kappe,",
  "thag2.49:2.2_0": "yaṁ saññamalabhiṁ tadā;",
  "thag2.49:2.3_0": "Tassā saññāya vāhasā,",
  "thag2.49:2.4_0": "patto me āsavakkhayo”ti.",
  "thag2.49:2.5_0": "…",
  "thag2.49:2.6_0": "Sandhito thero ….",
  "thag2.49:2.7_0": "vaggo pañcamo.",
  "thag2.49:3.0_0": "Tassuddānaṁ",
  "thag2.49:3.1_0": "Kumārakassapo thero,",
  "thag2.49:3.2_0": "dhammapālo ca brahmāli;",
  "thag2.49:3.3_0": "Mogharājā visākho ca,",
  "thag2.49:3.4_0": "cūḷako ca anūpamo;",
  "thag2.49:3.5_0": "Vajjito sandhito thero,",
  "thag2.49:3.6_0": "kilesarajavāhanoti.",
  "thag2.49:3.7_0": "Dukanipāto niṭṭhito.",
  "thag2.49:4.0_0": "Tatruddānaṁ",
  "thag2.49:4.1_0": "Gāthādukanipātamhi,",
  "thag2.49:4.2_0": "navuti ceva aṭṭha ca;",
  "thag2.49:4.3_0": "Therā ekūnapaññāsaṁ,",
  "thag2.49:4.4_0": "bhāsitā nayakovidāti."
}