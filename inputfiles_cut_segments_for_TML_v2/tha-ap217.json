{
  "tha-ap217:0.1_0": "Therāpadāna",
  "tha-ap217:0.2_0": "Hatthivagga",
  "tha-ap217:0.3_0": "5. Raṁsisaññakattheraapadāna",
  "tha-ap217:1.1_0": "“Udentaṁ sataraṁsiṁva,",
  "tha-ap217:1.2_0": "pītaraṁsiṁva bhāṇumaṁ;",
  "tha-ap217:1.3_0": "Byagghūsabhaṁva pavaraṁ,",
  "tha-ap217:1.4_0": "sujātaṁ pabbatantare.",
  "tha-ap217:2.1_0": "Buddhassa ānubhāvo so,",
  "tha-ap217:2.2_0": "jalate pabbatantare;",
  "tha-ap217:2.3_0": "Raṁse cittaṁ pasādetvā,",
  "tha-ap217:2.4_0": "kappaṁ saggamhi modahaṁ.",
  "tha-ap217:3.1_0": "Avasesesu kappesu,",
  "tha-ap217:3.2_0": "kusalaṁ caritaṁ mayā;",
  "tha-ap217:3.3_0": "Tena cittappasādena,",
  "tha-ap217:3.4_0": "buddhānussatiyāpi ca.",
  "tha-ap217:4.1_0": "Tiṁsakappasahasseto,",
  "tha-ap217:4.2_0": "yaṁ saññamalabhiṁ tadā;",
  "tha-ap217:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap217:4.4_0": "buddhasaññāyidaṁ phalaṁ.",
  "tha-ap217:5.1_0": "Sattapaññāsakappamhi,",
  "tha-ap217:5.2_0": "eko āsiṁ janādhipo;",
  "tha-ap217:5.3_0": "Sujāto nāma nāmena,",
  "tha-ap217:5.4_0": "cakkavattī mahabbalo.",
  "tha-ap217:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap217:6.2_0": "…pe…",
  "tha-ap217:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap217:7.1_0": "Itthaṁ sudaṁ āyasmā raṁsisaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap217:8.1_0": "Raṁsisaññakattherassāpadānaṁ pañcamaṁ."
}