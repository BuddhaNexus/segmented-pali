{
  "ja98:0.1_0": "Jātaka",
  "ja98:0.2_0": "Ekakanipāta",
  "ja98:0.3_0": "Littavagga",
  "ja98:0.4_0": "8. Kūṭavāṇijajātaka",
  "ja98:1.1_0": "“Sādhu kho paṇḍito nāma,",
  "ja98:1.2_0": "na tveva atipaṇḍito;",
  "ja98:1.3_0": "Atipaṇḍitena puttena,",
  "ja98:1.4_0": "manamhi upakūḷitoti.",
  "ja98:2.1_0": "Kūṭavāṇijajātakaṁ aṭṭhamaṁ."
}