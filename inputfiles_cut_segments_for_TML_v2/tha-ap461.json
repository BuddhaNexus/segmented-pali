{
  "tha-ap461:0.1_0": "Therāpadāna",
  "tha-ap461:0.2_0": "Jagatidāyakavagga",
  "tha-ap461:0.3_0": "9 Paṭṭipupphiyattheraapadāna",
  "tha-ap461:1.1_0": "“Nīharante sarīramhi,",
  "tha-ap461:1.2_0": "vajjamānāsu bherisu;",
  "tha-ap461:1.3_0": "Pasannacitto sumano,",
  "tha-ap461:1.4_0": "paṭṭipupphamapūjayiṁ.",
  "tha-ap461:2.1_0": "Satasahassito kappe,",
  "tha-ap461:2.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap461:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap461:2.4_0": "dehapūjāyidaṁ phalaṁ.",
  "tha-ap461:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap461:3.2_0": "…pe…",
  "tha-ap461:3.3_0": "viharāmi anāsavo.",
  "tha-ap461:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap461:4.2_0": "…pe…",
  "tha-ap461:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap461:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap461:5.2_0": "…pe…",
  "tha-ap461:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap461:6.1_0": "Itthaṁ sudaṁ āyasmā paṭṭipupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap461:7.1_0": "Paṭṭipupphiyattherassāpadānaṁ navamaṁ."
}