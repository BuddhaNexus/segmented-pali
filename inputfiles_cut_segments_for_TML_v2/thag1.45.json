{
  "thag1.45:0.1_0": "Theragāthā",
  "thag1.45:0.2_0": "Ekakanipāta",
  "thag1.45:0.3_0": "Pañcamavagga",
  "thag1.45:0.4_0": "1.45. Ramaṇīyavihārittheragāthā",
  "thag1.45:1.1_0": "“Yathāpi bhaddo ājañño,",
  "thag1.45:1.2_0": "khalitvā patitiṭṭhati;",
  "thag1.45:1.3_0": "Evaṁ dassanasampannaṁ,",
  "thag1.45:1.4_0": "sammāsambuddhasāvakan”ti.",
  "thag1.45:1.5_0": "… Ramaṇīyavihāritthero …."
}