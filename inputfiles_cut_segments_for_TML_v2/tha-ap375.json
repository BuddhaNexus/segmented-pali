{
  "tha-ap375:0.1_0": "Therāpadāna",
  "tha-ap375:0.2_0": "Bodhivandanavagga",
  "tha-ap375:0.3_0": "3 Tīṇuppalamāliyattheraapadāna",
  "tha-ap375:1.1_0": "“Candabhāgānadītīre,",
  "tha-ap375:1.2_0": "ahosiṁ vānaro tadā;",
  "tha-ap375:1.3_0": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap375:1.4_0": "nisinnaṁ pabbatantare.",
  "tha-ap375:2.1_0": "Obhāsentaṁ disā sabbā,",
  "tha-ap375:2.2_0": "sālarājaṁva phullitaṁ;",
  "tha-ap375:2.3_0": "Lakkhaṇabyañjanūpetaṁ,",
  "tha-ap375:2.4_0": "disvānattamano ahaṁ.",
  "tha-ap375:3.1_0": "Udaggacitto sumano,",
  "tha-ap375:3.2_0": "pītiyā haṭṭhamānaso;",
  "tha-ap375:3.3_0": "Tīṇi uppalapupphāni,",
  "tha-ap375:3.4_0": "matthake abhiropayiṁ.",
  "tha-ap375:4.1_0": "Pūjayitvāna pupphāni,",
  "tha-ap375:4.2_0": "phussassāhaṁ mahesino;",
  "tha-ap375:4.3_0": "Sagāravo bhavitvāna,",
  "tha-ap375:4.4_0": "pakkāmiṁ uttarāmukho.",
  "tha-ap375:5.1_0": "Gacchanto paṭikuṭiko,",
  "tha-ap375:5.2_0": "vippasannena cetasā;",
  "tha-ap375:5.3_0": "Selantare patitvāna,",
  "tha-ap375:5.4_0": "pāpuṇiṁ jīvitakkhayaṁ.",
  "tha-ap375:6.1_0": "Tena kammena sukatena,",
  "tha-ap375:6.2_0": "cetanāpaṇidhīhi ca;",
  "tha-ap375:6.3_0": "Jahitvā purimaṁ jātiṁ,",
  "tha-ap375:6.4_0": "tāvatiṁsamagacchahaṁ.",
  "tha-ap375:7.1_0": "Satānaṁ tīṇikkhattuñca,",
  "tha-ap375:7.2_0": "devarajjamakārayiṁ;",
  "tha-ap375:7.3_0": "Satānaṁ pañcakkhattuñca,",
  "tha-ap375:7.4_0": "cakkavattī ahosahaṁ.",
  "tha-ap375:8.1_0": "Dvenavute ito kappe,",
  "tha-ap375:8.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap375:8.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap375:8.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap375:9.1_0": "Paṭisambhidā catasso,",
  "tha-ap375:9.2_0": "…pe…",
  "tha-ap375:9.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap375:10.1_0": "Itthaṁ sudaṁ āyasmā tīṇuppalamāliyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap375:11.1_0": "Tīṇuppalamāliyattherassāpadānaṁ tatiyaṁ."
}