{
  "ja264:0.1_0": "Jātaka",
  "ja264:0.2_0": "Tikanipāta",
  "ja264:0.3_0": "Padumavagga",
  "ja264:0.4_0": "4. Mahāpanādajātaka",
  "ja264:1.1_0": "“Panādo nāma so rājā,",
  "ja264:1.2_0": "yassa yūpo suvaṇṇayo;",
  "ja264:1.3_0": "Tiriyaṁ soḷasubbedho,",
  "ja264:1.4_0": "uddhamāhu sahassadhā.",
  "ja264:2.1_0": "Sahassakaṇḍo satageṇḍu,",
  "ja264:2.2_0": "dhajālu haritāmayo;",
  "ja264:2.3_0": "Anaccuṁ tattha gandhabbā,",
  "ja264:2.4_0": "cha sahassāni sattadhā.",
  "ja264:3.1_0": "Evametaṁ tadā āsi,",
  "ja264:3.2_0": "yathā bhāsasi bhaddaji;",
  "ja264:3.3_0": "Sakko ahaṁ tadā āsiṁ,",
  "ja264:3.4_0": "veyyāvaccakaro tavā”ti.",
  "ja264:4.1_0": "Mahāpanādajātakaṁ catutthaṁ."
}