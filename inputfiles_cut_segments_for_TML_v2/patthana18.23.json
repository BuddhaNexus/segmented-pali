{
  "patthana18.23:0.1_0": "Paṭṭhānapakaraṇa",
  "patthana18.23:0.2_0": "Dhammānulomapaccanīya (3), Dukadukapaṭṭhānapāḷi (6)",
  "patthana18.23:0.3_0": "18.23. Upādānagocchakādi, Hetuduka",
  "patthana18.23:1.1_0": "Upādānaṁ hetuṁ dhammaṁ paṭicca noupādāno nahetu dhammo uppajjati hetupaccayā.",
  "patthana18.23:1.1_1": "(Saṅkhittaṁ.) Hetuyā cattāri …pe…",
  "patthana18.23:1.1_2": "vipāke ekaṁ …pe…",
  "patthana18.23:1.1_3": "avigate cattāri.",
  "patthana18.23:2.1_0": "Kilesaṁ hetuṁ dhammaṁ paṭicca nokileso nahetu dhammo uppajjati hetupaccayā.",
  "patthana18.23:2.1_1": "(Saṅkhittaṁ.) Hetuyā cattāri …pe…",
  "patthana18.23:2.1_2": "vipāke ekaṁ …pe…",
  "patthana18.23:2.1_3": "avigate cattāri.",
  "patthana18.23:2.1_4": "(Saṅkhittaṁ.)"
}