{
  "tha-ap303:0.1_0": "Therāpadāna",
  "tha-ap303:0.2_0": "Padumakesaravagga",
  "tha-ap303:0.3_0": "1. Padumakesariyattheraapadāna",
  "tha-ap303:1.1_0": "“Isisaṅghe ahaṁ pubbe,",
  "tha-ap303:1.2_0": "āsiṁ mātaṅgavāraṇo;",
  "tha-ap303:1.3_0": "Mahesīnaṁ pasādena,",
  "tha-ap303:1.4_0": "padmakesaramokiriṁ.",
  "tha-ap303:2.1_0": "Paccekajinaseṭṭhesu,",
  "tha-ap303:2.2_0": "dhutarāgesu tādisu;",
  "tha-ap303:2.3_0": "Tesu cittaṁ pasādetvā,",
  "tha-ap303:2.4_0": "kappaṁ saggamhi modahaṁ.",
  "tha-ap303:3.1_0": "Ekanavutito kappe,",
  "tha-ap303:3.2_0": "kesaraṁ okiriṁ tadā;",
  "tha-ap303:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap303:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap303:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap303:4.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap303:4.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap303:4.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap303:5.1_0": "Itthaṁ sudaṁ āyasmā padumakesariyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap303:6.1_0": "Padumakesariyattherassāpadānaṁ paṭhamaṁ."
}