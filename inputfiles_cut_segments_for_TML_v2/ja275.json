{
  "ja275:0.1_0": "Jātaka",
  "ja275:0.2_0": "Tikanipāta",
  "ja275:0.3_0": "Udapānavagga",
  "ja275:0.4_0": "5. Rucirajātaka",
  "ja275:1.1_0": "“Kāyaṁ balākā rucirā,",
  "ja275:1.2_0": "kākanīḷasmimacchati;",
  "ja275:1.3_0": "Caṇḍo kāko sakhā mayhaṁ,",
  "ja275:1.4_0": "yassa cetaṁ kulāvakaṁ”.",
  "ja275:2.1_0": "“Nanu maṁ samma jānāsi,",
  "ja275:2.2_0": "dija sāmākabhojana;",
  "ja275:2.3_0": "Akatvā vacanaṁ tuyhaṁ,",
  "ja275:2.4_0": "passa lūnosmi āgato”.",
  "ja275:3.1_0": "“Punapāpajjasī samma,",
  "ja275:3.2_0": "sīlañhi tava tādisaṁ;",
  "ja275:3.3_0": "Na hi mānusakā bhogā,",
  "ja275:3.4_0": "subhuñjā honti pakkhinā”ti.",
  "ja275:4.1_0": "Rucirajātakaṁ pañcamaṁ."
}