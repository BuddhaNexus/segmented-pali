{
  "cp28:0.1_0": "Cariyāpiṭaka",
  "cp28:0.2_0": "Yudhañjayavagga",
  "cp28:0.3_0": "Saccapāramī 2",
  "cp28:0.4_0": "Saccatāpasacariya",
  "cp28:1.1_0": "“Punāparaṁ yadā homi,",
  "cp28:1.2_0": "Tāpaso saccasavhayo;",
  "cp28:1.3_0": "Saccena lokaṁ pālesiṁ,",
  "cp28:1.4_0": "Samaggaṁ janamakāsahan”ti.",
  "cp28:2.1_0": "Saccatāpasacariyaṁ aṭṭhamaṁ."
}