{
  "pv45:0.1_0": "Petavatthu",
  "pv45:0.2_0": "Mahāvagga",
  "pv45:0.3_0": "10. Gaṇapetavatthu",
  "pv45:1.1_0": "“Naggā dubbaṇṇarūpāttha,",
  "pv45:1.2_0": "kisā dhamanisanthatā;",
  "pv45:1.3_0": "Upphāsulikā kisikā,",
  "pv45:1.4_0": "ke nu tumhettha mārisā”ti.",
  "pv45:2.1_0": "“Mayaṁ bhadante petāmhā,",
  "pv45:2.2_0": "duggatā yamalokikā;",
  "pv45:2.3_0": "Pāpakammaṁ karitvāna,",
  "pv45:2.4_0": "petalokaṁ ito gatā”ti.",
  "pv45:3.1_0": "“Kiṁ nu kāyena vācāya,",
  "pv45:3.2_0": "manasā dukkaṭaṁ kataṁ;",
  "pv45:3.3_0": "Kissa kammavipākena,",
  "pv45:3.4_0": "petalokaṁ ito gatā”ti.",
  "pv45:4.1_0": "“Anāvaṭesu titthesu,",
  "pv45:4.2_0": "vicinimhaddhamāsakaṁ;",
  "pv45:4.3_0": "Santesu deyyadhammesu,",
  "pv45:4.4_0": "dīpaṁ nākamha attano.",
  "pv45:5.1_0": "Nadiṁ upema tasitā,",
  "pv45:5.2_0": "rittakā parivattati;",
  "pv45:5.3_0": "Chāyaṁ upema uṇhesu,",
  "pv45:5.4_0": "ātapo parivattati.",
  "pv45:6.1_0": "Aggivaṇṇo ca no vāto,",
  "pv45:6.2_0": "ḍahanto upavāyati;",
  "pv45:6.3_0": "Etañca bhante arahāma,",
  "pv45:6.4_0": "aññañca pāpakaṁ tato.",
  "pv45:7.1_0": "Api yojanāni gacchāma,",
  "pv45:7.2_0": "chātā āhāragedhino;",
  "pv45:7.3_0": "Aladdhāva nivattāma,",
  "pv45:7.4_0": "aho no appapuññatā.",
  "pv45:8.1_0": "Chātā pamucchitā bhantā,",
  "pv45:8.2_0": "bhūmiyaṁ paṭisumbhitā;",
  "pv45:8.3_0": "Uttānā paṭikirāma,",
  "pv45:8.4_0": "avakujjā patāmase.",
  "pv45:9.1_0": "Te ca tattheva patitā,",
  "pv45:9.2_0": "bhūmiyaṁ paṭisumbhitā;",
  "pv45:9.3_0": "Uraṁ sīsañca ghaṭṭema,",
  "pv45:9.4_0": "aho no appapuññatā.",
  "pv45:10.1_0": "Etañca bhante arahāma,",
  "pv45:10.2_0": "aññañca pāpakaṁ tato;",
  "pv45:10.3_0": "Santesu deyyadhammesu,",
  "pv45:10.4_0": "dīpaṁ nākamha attano.",
  "pv45:11.1_0": "Te hi nūna ito gantvā,",
  "pv45:11.2_0": "yoniṁ laddhāna mānusiṁ;",
  "pv45:11.3_0": "Vadaññū sīlasampannā,",
  "pv45:11.4_0": "kāhāma kusalaṁ bahun”ti.",
  "pv45:12.1_0": "Gaṇapetavatthu dasamaṁ."
}