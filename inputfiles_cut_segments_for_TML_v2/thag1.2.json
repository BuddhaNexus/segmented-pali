{
  "thag1.2:0.1_0": "Theragāthā",
  "thag1.2:0.2_0": "Ekakanipāta",
  "thag1.2:0.3_0": "Paṭhamavagga",
  "thag1.2:0.4_0": "1.2. Mahākoṭṭhikattheragāthā",
  "thag1.2:1.1_0": "“Upasanto uparato,",
  "thag1.2:1.2_0": "mantabhāṇī anuddhato;",
  "thag1.2:1.3_0": "Dhunāti pāpake dhamme,",
  "thag1.2:1.4_0": "dumapattaṁva māluto”ti.",
  "thag1.2:1.5_0": "Itthaṁ sudaṁ āyasmā mahākoṭṭhiko thero gāthaṁ abhāsitthāti."
}