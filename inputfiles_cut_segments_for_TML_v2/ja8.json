{
  "ja8:0.1_0": "Jātaka",
  "ja8:0.2_0": "Ekakanipāta",
  "ja8:0.3_0": "Apaṇṇakavagga",
  "ja8:0.4_0": "8. Gāmaṇijātaka",
  "ja8:1.1_0": "“Api ataramānānaṁ,",
  "ja8:1.2_0": "phalāsāva samijjhati;",
  "ja8:1.3_0": "Vipakkabrahmacariyosmi,",
  "ja8:1.4_0": "evaṁ jānāhi gāmaṇī”ti.",
  "ja8:2.1_0": "Gāmaṇijātakaṁ aṭṭhamaṁ."
}