{
  "tha-ap552:0.1_0": "Therāpadāna",
  "tha-ap552:0.2_0": "Bhaddiyavagga",
  "tha-ap552:0.3_0": "10. Cūḷasugandhattheraapadāna",
  "tha-ap552:1.1_0": "“Imamhi bhaddake kappe,",
  "tha-ap552:1.2_0": "brahmabandhu mahāyaso;",
  "tha-ap552:1.3_0": "Kassapo nāma gottena,",
  "tha-ap552:1.4_0": "uppajji vadataṁ varo.",
  "tha-ap552:2.1_0": "Anubyañjanasampanno,",
  "tha-ap552:2.2_0": "bāttiṁsavaralakkhaṇo;",
  "tha-ap552:2.3_0": "Byāmappabhāparivuto,",
  "tha-ap552:2.4_0": "raṁsijālasamotthaṭo.",
  "tha-ap552:3.1_0": "Assāsetā yathā cando,",
  "tha-ap552:3.2_0": "sūriyova pabhaṅkaro;",
  "tha-ap552:3.3_0": "Nibbāpetā yathā megho,",
  "tha-ap552:3.4_0": "sāgarova guṇākaro.",
  "tha-ap552:4.1_0": "Dharaṇīriva sīlena,",
  "tha-ap552:4.2_0": "himavāva samādhinā;",
  "tha-ap552:4.3_0": "Ākāso viya paññāya,",
  "tha-ap552:4.4_0": "asaṅgo anilo yathā.",
  "tha-ap552:5.1_0": "Tadāhaṁ bārāṇasiyaṁ,",
  "tha-ap552:5.2_0": "upapanno mahākule;",
  "tha-ap552:5.3_0": "Pahūtadhanadhaññasmiṁ,",
  "tha-ap552:5.4_0": "nānāratanasañcaye.",
  "tha-ap552:6.1_0": "Mahatā parivārena,",
  "tha-ap552:6.2_0": "nisinnaṁ lokanāyakaṁ;",
  "tha-ap552:6.3_0": "Upecca dhammamassosiṁ,",
  "tha-ap552:6.4_0": "amataṁva manoharaṁ.",
  "tha-ap552:7.1_0": "Dvattiṁsalakkhaṇadharo,",
  "tha-ap552:7.2_0": "sanakkhattova candimā;",
  "tha-ap552:7.3_0": "Anubyañjanasampanno,",
  "tha-ap552:7.4_0": "sālarājāva phullito.",
  "tha-ap552:8.1_0": "Raṁsijālaparikkhitto,",
  "tha-ap552:8.2_0": "dittova kanakācalo;",
  "tha-ap552:8.3_0": "Byāmappabhāparivuto,",
  "tha-ap552:8.4_0": "sataraṁsī divākaro.",
  "tha-ap552:9.1_0": "Soṇṇānano jinavaro,",
  "tha-ap552:9.2_0": "samaṇīva siluccayo;",
  "tha-ap552:9.3_0": "Karuṇā puṇṇahadayo,",
  "tha-ap552:9.4_0": "guṇena viya sāgaro.",
  "tha-ap552:10.1_0": "Lokavissutakitti ca,",
  "tha-ap552:10.2_0": "sinerūva naguttamo;",
  "tha-ap552:10.3_0": "Yasasā vitthato vīro,",
  "tha-ap552:10.4_0": "ākāsasadiso muni.",
  "tha-ap552:11.1_0": "Asaṅgacitto sabbattha,",
  "tha-ap552:11.2_0": "anilo viya nāyako;",
  "tha-ap552:11.3_0": "Patiṭṭhā sabbabhūtānaṁ,",
  "tha-ap552:11.4_0": "mahīva munisattamo.",
  "tha-ap552:12.1_0": "Anupalitto lokena,",
  "tha-ap552:12.2_0": "toyena padumaṁ yathā;",
  "tha-ap552:12.3_0": "Kuvādagacchadahano,",
  "tha-ap552:12.4_0": "aggikhandhova sobhasi.",
  "tha-ap552:13.1_0": "Agadho viya sabbattha,",
  "tha-ap552:13.2_0": "kilesavisanāsako;",
  "tha-ap552:13.3_0": "Gandhamādanaselova,",
  "tha-ap552:13.4_0": "guṇagandhavibhūsito.",
  "tha-ap552:14.1_0": "Guṇānaṁ ākaro vīro,",
  "tha-ap552:14.2_0": "ratanānaṁva sāgaro;",
  "tha-ap552:14.3_0": "Sindhūva vanarājīnaṁ,",
  "tha-ap552:14.4_0": "kilesamalahārako.",
  "tha-ap552:15.1_0": "Vijayīva mahāyodho,",
  "tha-ap552:15.2_0": "mārasenāvamaddano;",
  "tha-ap552:15.3_0": "Cakkavattīva so rājā,",
  "tha-ap552:15.4_0": "bojjhaṅgaratanissaro.",
  "tha-ap552:16.1_0": "Mahābhisakkasaṅkāso,",
  "tha-ap552:16.2_0": "dosabyādhitikicchako;",
  "tha-ap552:16.3_0": "Sallakatto yathā vejjo,",
  "tha-ap552:16.4_0": "diṭṭhigaṇḍaviphālako.",
  "tha-ap552:17.1_0": "So tadā lokapajjoto,",
  "tha-ap552:17.2_0": "sanarāmarasakkato;",
  "tha-ap552:17.3_0": "Parisāsu narādicco,",
  "tha-ap552:17.4_0": "dhammaṁ desayate jino.",
  "tha-ap552:18.1_0": "‘Dānaṁ datvā mahābhogo,",
  "tha-ap552:18.2_0": "sīlena sugatūpago;",
  "tha-ap552:18.3_0": "Bhāvanāya ca nibbāti’,",
  "tha-ap552:18.4_0": "iccevamanusāsatha.",
  "tha-ap552:19.1_0": "Desanaṁ taṁ mahassādaṁ,",
  "tha-ap552:19.2_0": "ādimajjhantasobhanaṁ;",
  "tha-ap552:19.3_0": "Suṇanti parisā sabbā,",
  "tha-ap552:19.4_0": "amataṁva mahārasaṁ.",
  "tha-ap552:20.1_0": "Sutvā sumadhuraṁ dhammaṁ,",
  "tha-ap552:20.2_0": "pasanno jinasāsane;",
  "tha-ap552:20.3_0": "Sugataṁ saraṇaṁ gantvā,",
  "tha-ap552:20.4_0": "yāvajīvaṁ namassahaṁ.",
  "tha-ap552:21.1_0": "Munino gandhakuṭiyā,",
  "tha-ap552:21.2_0": "opuñjesiṁ tadā mahiṁ;",
  "tha-ap552:21.3_0": "Catujjātena gandhena,",
  "tha-ap552:21.4_0": "māse aṭṭha dinesvahaṁ.",
  "tha-ap552:22.1_0": "Paṇidhāya sugandhattaṁ,",
  "tha-ap552:22.2_0": "sarīravissagandhino;",
  "tha-ap552:22.3_0": "Tadā jino viyākāsi,",
  "tha-ap552:22.4_0": "sugandhatanulābhitaṁ.",
  "tha-ap552:23.1_0": "‘Yo yaṁ gandhakuṭibhūmiṁ,",
  "tha-ap552:23.2_0": "gandhenopuñjate sakiṁ;",
  "tha-ap552:23.3_0": "Tena kammavipākena,",
  "tha-ap552:23.4_0": "upapanno tahiṁ tahiṁ.",
  "tha-ap552:24.1_0": "Sugandhadeho sabbattha,",
  "tha-ap552:24.2_0": "bhavissati ayaṁ naro;",
  "tha-ap552:24.3_0": "Guṇagandhayutto hutvā,",
  "tha-ap552:24.4_0": "nibbāyissatināsavo’.",
  "tha-ap552:25.1_0": "Tena kammena sukatena,",
  "tha-ap552:25.2_0": "cetanāpaṇidhīhi ca;",
  "tha-ap552:25.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "tha-ap552:25.4_0": "tāvatiṁsamagacchahaṁ.",
  "tha-ap552:26.1_0": "Pacchime ca bhave dāni,",
  "tha-ap552:26.2_0": "jāto vippakule ahaṁ;",
  "tha-ap552:26.3_0": "Gabbhaṁ me vasato mātā,",
  "tha-ap552:26.4_0": "dehenāsi sugandhitā.",
  "tha-ap552:27.1_0": "Yadā ca mātukucchimhā,",
  "tha-ap552:27.2_0": "nikkhamāmi tadā purī;",
  "tha-ap552:27.3_0": "Sāvatthisabbagandhehi,",
  "tha-ap552:27.4_0": "vāsitā viya vāyatha.",
  "tha-ap552:28.1_0": "Pupphavassañca surabhi,",
  "tha-ap552:28.2_0": "dibbagandhaṁ manoramaṁ;",
  "tha-ap552:28.3_0": "Dhūpāni ca mahagghāni,",
  "tha-ap552:28.4_0": "upavāyiṁsu tāvade.",
  "tha-ap552:29.1_0": "Devā ca sabbagandhehi,",
  "tha-ap552:29.2_0": "dhūpapupphehi taṁ gharaṁ;",
  "tha-ap552:29.3_0": "Vāsayiṁsu sugandhena,",
  "tha-ap552:29.4_0": "yasmiṁ jāto ahaṁ ghare.",
  "tha-ap552:30.1_0": "Yadā ca taruṇo bhaddo,",
  "tha-ap552:30.2_0": "paṭhame yobbane ṭhito;",
  "tha-ap552:30.3_0": "Tadā selaṁ saparisaṁ,",
  "tha-ap552:30.4_0": "vinetvā narasārathi.",
  "tha-ap552:31.1_0": "Tehi sabbehi parivuto,",
  "tha-ap552:31.2_0": "sāvatthipuramāgato;",
  "tha-ap552:31.3_0": "Tadā buddhānubhāvaṁ taṁ,",
  "tha-ap552:31.4_0": "disvā pabbajito ahaṁ.",
  "tha-ap552:32.1_0": "Sīlaṁ samādhipaññañca,",
  "tha-ap552:32.2_0": "vimuttiñca anuttaraṁ;",
  "tha-ap552:32.3_0": "Bhāvetvā caturo dhamme,",
  "tha-ap552:32.4_0": "pāpuṇiṁ āsavakkhayaṁ.",
  "tha-ap552:33.1_0": "Yadā pabbajito cāhaṁ,",
  "tha-ap552:33.2_0": "yadā ca arahā ahuṁ;",
  "tha-ap552:33.3_0": "Nibbāyissaṁ yadā cāhaṁ,",
  "tha-ap552:33.4_0": "gandhavasso tadā ahu.",
  "tha-ap552:34.1_0": "Sarīragandho ca sadātiseti me,",
  "tha-ap552:34.2_0": "Mahārahaṁ candanacampakuppalaṁ;",
  "tha-ap552:34.3_0": "Tatheva gandhe itare ca sabbaso,",
  "tha-ap552:34.4_0": "Pasayha vāyāmi tato tahiṁ tahiṁ.",
  "tha-ap552:35.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap552:35.2_0": "…pe…",
  "tha-ap552:35.3_0": "viharāmi anāsavo.",
  "tha-ap552:36.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap552:36.2_0": "…pe…",
  "tha-ap552:36.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap552:37.1_0": "Paṭisambhidā catasso,",
  "tha-ap552:37.2_0": "…pe…",
  "tha-ap552:37.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap552:38.1_0": "Itthaṁ sudaṁ āyasmā cūḷasugandho thero imā gāthāyo abhāsitthāti.",
  "tha-ap552:39.1_0": "Cūḷasugandhattherassāpadānaṁ dasamaṁ.",
  "tha-ap552:40.1_0": "Bhaddiyavaggo pañcapaññāsamo.",
  "tha-ap552:41.1_0": "Tassuddānaṁ",
  "tha-ap552:42.1_0": "Bhaddiyo revato thero,",
  "tha-ap552:42.2_0": "mahālābhī ca sīvali;",
  "tha-ap552:42.3_0": "Vaṅgīso nandako ceva,",
  "tha-ap552:42.4_0": "kāḷudāyī tathābhayo.",
  "tha-ap552:43.1_0": "Lomaso vanavaccho ca,",
  "tha-ap552:43.2_0": "sugandho ceva dasamo;",
  "tha-ap552:43.3_0": "Tīṇi gāthāsatā tattha,",
  "tha-ap552:43.4_0": "soḷasā ca taduttari.",
  "tha-ap552:44.1_0": "Atha vagguddānaṁ",
  "tha-ap552:45.1_0": "Kaṇikāravhayo vaggo,",
  "tha-ap552:45.2_0": "phalado tiṇadāyako;",
  "tha-ap552:45.3_0": "Kaccāno bhaddiyo vaggo,",
  "tha-ap552:45.4_0": "gāthāyo gaṇitā cimā.",
  "tha-ap552:46.1_0": "Navagāthāsatānīha,",
  "tha-ap552:46.2_0": "caturāsītiyeva ca;",
  "tha-ap552:46.3_0": "Sapaññāsaṁ pañcasataṁ,",
  "tha-ap552:46.4_0": "apadānā pakāsitā.",
  "tha-ap552:47.1_0": "Saha udānagāthāhi,",
  "tha-ap552:47.2_0": "chasahassāni hontimā;",
  "tha-ap552:47.3_0": "Dvesatāni ca gāthānaṁ,",
  "tha-ap552:47.4_0": "aṭṭhārasa taduttari."
}