{
  "tha-ap458:0.1_0": "Therāpadāna",
  "tha-ap458:0.2_0": "Jagatidāyakavagga",
  "tha-ap458:0.3_0": "6. Vanakoraṇḍiyattheraapadāna",
  "tha-ap458:1.1_0": "“Siddhatthassa bhagavato,",
  "tha-ap458:1.2_0": "lokajeṭṭhassa tādino;",
  "tha-ap458:1.3_0": "Vanakoraṇḍamādāya,",
  "tha-ap458:1.4_0": "buddhassa abhiropayiṁ.",
  "tha-ap458:2.1_0": "Catunnavutito kappe,",
  "tha-ap458:2.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap458:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap458:2.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap458:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap458:3.2_0": "…pe…",
  "tha-ap458:3.3_0": "viharāmi anāsavo.",
  "tha-ap458:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap458:4.2_0": "…pe…",
  "tha-ap458:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap458:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap458:5.2_0": "…pe…",
  "tha-ap458:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap458:6.1_0": "Itthaṁ sudaṁ āyasmā vanakoraṇḍiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap458:7.1_0": "Vanakoraṇḍiyattherassāpadānaṁ chaṭṭhaṁ.",
  "tha-ap458:7.2_0": "Vīsatimaṁ bhāṇavāraṁ."
}