{
  "ja232:0.1_0": "Jātaka",
  "ja232:0.2_0": "Dukanipāta",
  "ja232:0.3_0": "Upāhanavagga",
  "ja232:0.4_0": "2. Vīṇāguṇajātaka",
  "ja232:1.1_0": "“Ekacintito yamattho,",
  "ja232:1.2_0": "bālo apariṇāyako;",
  "ja232:1.3_0": "Na hi khujjena vāmena,",
  "ja232:1.4_0": "bhoti saṅgantumarahasi”.",
  "ja232:2.1_0": "“Purisūsabhaṁ maññamānā,",
  "ja232:2.2_0": "ahaṁ khujjamakāmayiṁ;",
  "ja232:2.3_0": "Soyaṁ saṅkuṭito seti,",
  "ja232:2.4_0": "chinnatanti yathā viṇā”ti.",
  "ja232:3.1_0": "Vīṇāguṇajātakaṁ dutiyaṁ."
}