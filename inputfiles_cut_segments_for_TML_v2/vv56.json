{
  "vv56:0.1_0": "Vimānavatthu",
  "vv56:0.2_0": "Purisavimāna",
  "vv56:0.3_0": "Mahārathavagga",
  "vv56:0.4_0": "6. Paṭhamakaraṇīyavimānavatthu",
  "vv56:1.1_0": "“Uccamidaṁ maṇithūṇaṁ vimānaṁ,",
  "vv56:1.2_0": "Samantato dvādasa yojanāni;",
  "vv56:1.3_0": "Kūṭāgārā sattasatā uḷārā,",
  "vv56:1.4_0": "Veḷuriyathambhā rucakatthatā subhā.",
  "vv56:2.1_0": "Tatthacchasi pivasi khādasi ca,",
  "vv56:2.2_0": "Dibbā ca vīṇā pavadanti vagguṁ;",
  "vv56:2.3_0": "Dibbā rasā kāmaguṇettha pañca,",
  "vv56:2.4_0": "Nāriyo ca naccanti suvaṇṇachannā.",
  "vv56:3.1_0": "Kena tetādiso vaṇṇo,",
  "vv56:3.2_0": "…pe…",
  "vv56:3.3_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv56:4.1_0": "So devaputto attamano,",
  "vv56:4.2_0": "…pe…",
  "vv56:4.3_0": "yassa kammassidaṁ phalaṁ.",
  "vv56:5.1_0": "“Karaṇīyāni puññāni,",
  "vv56:5.2_0": "paṇḍitena vijānatā;",
  "vv56:5.3_0": "Sammaggatesu buddhesu,",
  "vv56:5.4_0": "yattha dinnaṁ mahapphalaṁ.",
  "vv56:6.1_0": "Atthāya vata me buddho,",
  "vv56:6.2_0": "araññā gāmamāgato;",
  "vv56:6.3_0": "Tattha cittaṁ pasādetvā,",
  "vv56:6.4_0": "tāvatiṁsūpago ahaṁ.",
  "vv56:7.1_0": "Tena metādiso vaṇṇo,",
  "vv56:7.2_0": "…pe…",
  "vv56:7.3_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv56:8.1_0": "Paṭhamakaraṇīyavimānaṁ chaṭṭhaṁ."
}