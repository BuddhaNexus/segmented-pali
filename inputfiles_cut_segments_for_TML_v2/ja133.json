{
  "ja133:0.1_0": "Jātaka",
  "ja133:0.2_0": "Ekakanipāta",
  "ja133:0.3_0": "Asampadānavagga",
  "ja133:0.4_0": "3. Ghatāsanajātaka",
  "ja133:1.1_0": "“Khemaṁ yahiṁ tattha arī udīrito,",
  "ja133:1.2_0": "Dakassa majjhe jalate ghatāsano;",
  "ja133:1.3_0": "Na ajja vāso mahiyā mahīruhe,",
  "ja133:1.4_0": "Disā bhajavho saraṇājja no bhayan”ti.",
  "ja133:2.1_0": "Ghatāsanajātakaṁ tatiyaṁ."
}