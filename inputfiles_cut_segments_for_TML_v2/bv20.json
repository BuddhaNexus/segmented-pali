{
  "bv20:0.1_0": "Buddhavaṁsa",
  "bv20:0.2_0": "20. Phussabuddhavaṁsa",
  "bv20:1.1_0": "“Tattheva maṇḍakappamhi,",
  "bv20:1.2_0": "ahu satthā anuttaro;",
  "bv20:1.3_0": "Anūpamo asamasamo,",
  "bv20:1.4_0": "phusso lokagganāyako.",
  "bv20:2.1_0": "Sopi sabbaṁ tamaṁ hantvā,",
  "bv20:2.2_0": "vijaṭetvā mahājaṭaṁ;",
  "bv20:2.3_0": "Sadevakaṁ tappayanto,",
  "bv20:2.4_0": "abhivassi amatambunā.",
  "bv20:3.1_0": "Dhammacakkaṁ pavattente,",
  "bv20:3.2_0": "phusse nakkhattamaṅgale;",
  "bv20:3.3_0": "Koṭisatasahassānaṁ,",
  "bv20:3.4_0": "paṭhamābhisamayo ahu.",
  "bv20:4.1_0": "Navutisatasahassānaṁ,",
  "bv20:4.2_0": "dutiyābhisamayo ahu;",
  "bv20:4.3_0": "Asītisatasahassānaṁ,",
  "bv20:4.4_0": "tatiyābhisamayo ahu.",
  "bv20:5.1_0": "Sannipātā tayo āsuṁ,",
  "bv20:5.2_0": "phussassāpi mahesino;",
  "bv20:5.3_0": "Khīṇāsavānaṁ vimalānaṁ,",
  "bv20:5.4_0": "santacittāna tādinaṁ.",
  "bv20:6.1_0": "Saṭṭhisatasahassānaṁ,",
  "bv20:6.2_0": "paṭhamo āsi samāgamo;",
  "bv20:6.3_0": "Paññāsasatasahassānaṁ,",
  "bv20:6.4_0": "dutiyo āsi samāgamo.",
  "bv20:7.1_0": "Cattārīsasatasahassānaṁ,",
  "bv20:7.2_0": "Tatiyo āsi samāgamo;",
  "bv20:7.3_0": "Anupādā vimuttānaṁ,",
  "bv20:7.4_0": "Vocchinnapaṭisandhinaṁ.",
  "bv20:8.1_0": "Ahaṁ tena samayena,",
  "bv20:8.2_0": "vijitāvī nāma khattiyo;",
  "bv20:8.3_0": "Chaḍḍayitvā mahārajjaṁ,",
  "bv20:8.4_0": "pabbajiṁ tassa santike.",
  "bv20:9.1_0": "Sopi maṁ buddho byākāsi,",
  "bv20:9.2_0": "phusso lokagganāyako;",
  "bv20:9.3_0": "‘Dvenavute ito kappe,",
  "bv20:9.4_0": "ayaṁ buddho bhavissati.",
  "bv20:10.1_0": "Padhānaṁ padahitvāna,",
  "bv20:10.2_0": "…pe…",
  "bv20:10.3_0": "hessāma sammukhā imaṁ’.",
  "bv20:11.1_0": "Tassāpi vacanaṁ sutvā,",
  "bv20:11.2_0": "bhiyyo cittaṁ pasādayiṁ;",
  "bv20:11.3_0": "Uttariṁ vatamadhiṭṭhāsiṁ,",
  "bv20:11.4_0": "dasapāramipūriyā.",
  "bv20:12.1_0": "Suttantaṁ vinayañcāpi,",
  "bv20:12.2_0": "navaṅgaṁ satthusāsanaṁ;",
  "bv20:12.3_0": "Sabbaṁ pariyāpuṇitvā,",
  "bv20:12.4_0": "sobhayiṁ jinasāsanaṁ.",
  "bv20:13.1_0": "Tatthappamatto viharanto,",
  "bv20:13.2_0": "brahmaṁ bhāvetva bhāvanaṁ;",
  "bv20:13.3_0": "Abhiññāsu pāramiṁ gantvā,",
  "bv20:13.4_0": "brahmalokamagañchahaṁ.",
  "bv20:14.1_0": "Kāsikaṁ nāma nagaraṁ,",
  "bv20:14.2_0": "jayaseno nāma khattiyo;",
  "bv20:14.3_0": "Sirimā nāma janikā,",
  "bv20:14.4_0": "phussassāpi mahesino.",
  "bv20:15.1_0": "Navavassasahassāni,",
  "bv20:15.2_0": "Agāraṁ ajjha so vasi;",
  "bv20:15.3_0": "Garuḷapakkha haṁsa suvaṇṇabhārā,",
  "bv20:15.4_0": "Tayo pāsādamuttamā.",
  "bv20:16.1_0": "Tiṁsaitthisahassāni,",
  "bv20:16.2_0": "nāriyo samalaṅkatā;",
  "bv20:16.3_0": "Kisāgotamī nāma nārī,",
  "bv20:16.4_0": "anūpamo nāma atrajo.",
  "bv20:17.1_0": "Nimitte caturo disvā,",
  "bv20:17.2_0": "hatthiyānena nikkhami;",
  "bv20:17.3_0": "Chamāsaṁ padhānacāraṁ,",
  "bv20:17.4_0": "acarī purisuttamo.",
  "bv20:18.1_0": "Brahmunā yācito santo,",
  "bv20:18.2_0": "phusso lokagganāyako;",
  "bv20:18.3_0": "Vatti cakkaṁ mahāvīro,",
  "bv20:18.4_0": "migadāye naruttamo.",
  "bv20:19.1_0": "Surakkhito dhammaseno,",
  "bv20:19.2_0": "ahesuṁ aggasāvakā;",
  "bv20:19.3_0": "Sabhiyo nāmupaṭṭhāko,",
  "bv20:19.4_0": "phussassāpi mahesino.",
  "bv20:20.1_0": "Cālā ca upacālā ca,",
  "bv20:20.2_0": "ahesuṁ aggasāvikā;",
  "bv20:20.3_0": "Bodhi tassa bhagavato,",
  "bv20:20.4_0": "āmaṇḍoti pavuccati.",
  "bv20:21.1_0": "Dhanañcayo visākho ca,",
  "bv20:21.2_0": "ahesuṁ aggupaṭṭhakā;",
  "bv20:21.3_0": "Padumā ceva nāgā ca,",
  "bv20:21.4_0": "ahesuṁ aggupaṭṭhikā.",
  "bv20:22.1_0": "Aṭṭhapaṇṇāsaratanaṁ,",
  "bv20:22.2_0": "sopi accuggato muni;",
  "bv20:22.3_0": "Sobhate sataraṁsīva,",
  "bv20:22.4_0": "uḷurājāva pūrito.",
  "bv20:23.1_0": "Navutivassasahassāni,",
  "bv20:23.2_0": "āyu vijjati tāvade;",
  "bv20:23.3_0": "Tāvatā tiṭṭhamāno so,",
  "bv20:23.4_0": "tāresi janataṁ bahuṁ.",
  "bv20:24.1_0": "Ovaditvā bahū satte,",
  "bv20:24.2_0": "santāretvā bahū jane;",
  "bv20:24.3_0": "Sopi satthā atulayaso,",
  "bv20:24.4_0": "nibbuto so sasāvako.",
  "bv20:25.1_0": "Phusso jinavaro satthā,",
  "bv20:25.2_0": "Senārāmamhi nibbuto;",
  "bv20:25.3_0": "Dhātuvitthārikaṁ āsi,",
  "bv20:25.4_0": "Tesu tesu padesato”ti.",
  "bv20:26.1_0": "Phussassa bhagavato vaṁso aṭṭhārasamo."
}