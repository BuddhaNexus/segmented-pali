{
  "iti61:0.1_0": "Itivuttaka 61",
  "iti61:0.2_0": "Tikanipāta",
  "iti61:0.3_0": "Dutiyavagga",
  "iti61:0.4_0": "Cakkhusutta",
  "iti61:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti61:2.1_0": "“Tīṇimāni, bhikkhave, cakkhūni.",
  "iti61:2.2_0": "Katamāni tīṇi?",
  "iti61:2.3_0": "Maṁsacakkhu, dibbacakkhu, paññācakkhu—",
  "iti61:2.4_0": "imāni kho, bhikkhave, tīṇi cakkhūnī”ti.",
  "iti61:2.5_0": "Etamatthaṁ bhagavā avoca.",
  "iti61:2.6_0": "Tatthetaṁ iti vuccati:",
  "iti61:3.1_0": "“Maṁsacakkhu dibbacakkhu,",
  "iti61:3.2_0": "paññācakkhu anuttaraṁ;",
  "iti61:3.3_0": "Etāni tīṇi cakkhūni,",
  "iti61:3.4_0": "akkhāsi purisuttamo.",
  "iti61:4.1_0": "Maṁsacakkhussa uppādo,",
  "iti61:4.2_0": "maggo dibbassa cakkhuno;",
  "iti61:4.3_0": "Yato ñāṇaṁ udapādi,",
  "iti61:4.4_0": "paññācakkhu anuttaraṁ;",
  "iti61:4.5_0": "Yassa cakkhussa paṭilābhā,",
  "iti61:4.6_0": "sabbadukkhā pamuccatī”ti.",
  "iti61:5.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti61:5.2_0": "Dutiyaṁ."
}