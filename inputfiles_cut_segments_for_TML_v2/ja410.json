{
  "ja410:0.1_0": "Jātaka",
  "ja410:0.2_0": "Sattakanipāta",
  "ja410:0.3_0": "Gandhāravagga",
  "ja410:0.4_0": "5. Somadattajātaka",
  "ja410:1.1_0": "“Yo maṁ pure paccuḍḍeti,",
  "ja410:1.2_0": "araññe dūramāyato;",
  "ja410:1.3_0": "So na dissati mātaṅgo,",
  "ja410:1.4_0": "somadatto kuhiṁ gato.",
  "ja410:2.1_0": "Ayaṁ vā so mato seti,",
  "ja410:2.2_0": "allasiṅgaṁva vacchito;",
  "ja410:2.3_0": "Bhumyā nipatito seti,",
  "ja410:2.4_0": "amarā vata kuñjaro”.",
  "ja410:3.1_0": "“Anagāriyupetassa,",
  "ja410:3.2_0": "vippamuttassa te sato;",
  "ja410:3.3_0": "Samaṇassa na taṁ sādhu,",
  "ja410:3.4_0": "yaṁ petamanusocasi”.",
  "ja410:4.1_0": "“Saṁvāsena have sakka,",
  "ja410:4.2_0": "manussassa migassa vā;",
  "ja410:4.3_0": "Hadaye jāyate pemaṁ,",
  "ja410:4.4_0": "taṁ na sakkā asocituṁ”.",
  "ja410:5.1_0": "“Mataṁ marissaṁ rodanti,",
  "ja410:5.2_0": "ye rudanti lapanti ca;",
  "ja410:5.3_0": "Tasmā tvaṁ isi mā rodi,",
  "ja410:5.4_0": "roditaṁ moghamāhu santo.",
  "ja410:6.1_0": "Kanditena have brahme,",
  "ja410:6.2_0": "mato peto samuṭṭhahe;",
  "ja410:6.3_0": "Sabbe saṅgamma rodāma,",
  "ja410:6.4_0": "aññamaññassa ñātake”.",
  "ja410:7.1_0": "“Ādittaṁ vata maṁ santaṁ,",
  "ja410:7.2_0": "ghatasittaṁva pāvakaṁ;",
  "ja410:7.3_0": "Vārinā viya osiñcaṁ,",
  "ja410:7.4_0": "sabbaṁ nibbāpaye daraṁ.",
  "ja410:8.1_0": "Abbahī vata me sallaṁ,",
  "ja410:8.2_0": "yamāsi hadayassitaṁ;",
  "ja410:8.3_0": "Yo me sokaparetassa,",
  "ja410:8.4_0": "puttasokaṁ apānudi.",
  "ja410:9.1_0": "Sohaṁ abbūḷhasallosmi,",
  "ja410:9.2_0": "vītasoko anāvilo;",
  "ja410:9.3_0": "Na socāmi na rodāmi,",
  "ja410:9.4_0": "tava sutvāna vāsavā”ti.",
  "ja410:10.1_0": "Somadattajātakaṁ pañcamaṁ."
}