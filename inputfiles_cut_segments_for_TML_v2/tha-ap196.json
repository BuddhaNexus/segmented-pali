{
  "tha-ap196:0.1_0": "Therāpadāna",
  "tha-ap196:0.2_0": "Tamālapupphiyavagga",
  "tha-ap196:0.3_0": "4. Asokapūjakattheraapadāna",
  "tha-ap196:1.1_0": "“Tivarāyaṁ pure ramme,",
  "tha-ap196:1.2_0": "rājuyyānaṁ ahu tadā;",
  "tha-ap196:1.3_0": "Uyyānapālo tatthāsiṁ,",
  "tha-ap196:1.4_0": "rañño baddhacaro ahaṁ.",
  "tha-ap196:2.1_0": "Padumo nāma nāmena,",
  "tha-ap196:2.2_0": "sayambhū sappabho ahu;",
  "tha-ap196:2.3_0": "Nisinnaṁ puṇḍarīkamhi,",
  "tha-ap196:2.4_0": "chāyā na jahi taṁ muniṁ.",
  "tha-ap196:3.1_0": "Asokaṁ pupphitaṁ disvā,",
  "tha-ap196:3.2_0": "piṇḍibhāraṁ sudassanaṁ;",
  "tha-ap196:3.3_0": "Buddhassa abhiropesiṁ,",
  "tha-ap196:3.4_0": "jalajuttamanāmino.",
  "tha-ap196:4.1_0": "Catunnavutito kappe,",
  "tha-ap196:4.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap196:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap196:4.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap196:5.1_0": "Sattatiṁsamhito kappe,",
  "tha-ap196:5.2_0": "soḷasa araṇañjahā;",
  "tha-ap196:5.3_0": "Sattaratanasampannā,",
  "tha-ap196:5.4_0": "cakkavattī mahabbalā.",
  "tha-ap196:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap196:6.2_0": "…pe…",
  "tha-ap196:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap196:7.1_0": "Itthaṁ sudaṁ āyasmā asokapūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap196:8.1_0": "Asokapūjakattherassāpadānaṁ catutthaṁ."
}