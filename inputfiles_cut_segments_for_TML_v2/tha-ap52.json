{
  "tha-ap52:0.1_0": "Therāpadāna",
  "tha-ap52:0.2_0": "Upālivagga",
  "tha-ap52:0.3_0": "10. Cundattheraapadāna",
  "tha-ap52:1.1_0": "“Siddhatthassa bhagavato,",
  "tha-ap52:1.2_0": "lokajeṭṭhassa tādino;",
  "tha-ap52:1.3_0": "Agghiyaṁ kārayitvāna,",
  "tha-ap52:1.4_0": "jātipupphehi chādayiṁ.",
  "tha-ap52:2.1_0": "Niṭṭhāpetvāna taṁ pupphaṁ,",
  "tha-ap52:2.2_0": "buddhassa upanāmayiṁ;",
  "tha-ap52:2.3_0": "Pupphāvasesaṁ paggayha,",
  "tha-ap52:2.4_0": "buddhassa abhiropayiṁ.",
  "tha-ap52:3.1_0": "Kañcanagghiyasaṅkāsaṁ,",
  "tha-ap52:3.2_0": "buddhaṁ lokagganāyakaṁ;",
  "tha-ap52:3.3_0": "Pasannacitto sumano,",
  "tha-ap52:3.4_0": "pupphagghiyamupānayiṁ.",
  "tha-ap52:4.1_0": "Vitiṇṇakaṅkho sambuddho,",
  "tha-ap52:4.2_0": "tiṇṇoghehi purakkhato;",
  "tha-ap52:4.3_0": "Bhikkhusaṅghe nisīditvā,",
  "tha-ap52:4.4_0": "imā gāthā abhāsatha.",
  "tha-ap52:5.1_0": "‘Dibbagandhaṁ pavāyantaṁ,",
  "tha-ap52:5.2_0": "yo me pupphagghiyaṁ adā;",
  "tha-ap52:5.3_0": "Tamahaṁ kittayissāmi,",
  "tha-ap52:5.4_0": "suṇātha mama bhāsato.",
  "tha-ap52:6.1_0": "Ito cuto ayaṁ poso,",
  "tha-ap52:6.2_0": "devasaṅghapurakkhato;",
  "tha-ap52:6.3_0": "Jātipupphehi parikiṇṇo,",
  "tha-ap52:6.4_0": "devalokaṁ gamissati.",
  "tha-ap52:7.1_0": "Ubbiddhaṁ bhavanaṁ tassa,",
  "tha-ap52:7.2_0": "Sovaṇṇañca maṇīmayaṁ;",
  "tha-ap52:7.3_0": "Byamhaṁ pātubhavissati,",
  "tha-ap52:7.4_0": "Puññakammappabhāvitaṁ.",
  "tha-ap52:8.1_0": "Catusattatikkhattuṁ so,",
  "tha-ap52:8.2_0": "devarajjaṁ karissati;",
  "tha-ap52:8.3_0": "Anubhossati sampattiṁ,",
  "tha-ap52:8.4_0": "accharāhi purakkhato.",
  "tha-ap52:9.1_0": "Pathabyā rajjaṁ tisataṁ,",
  "tha-ap52:9.2_0": "vasudhaṁ āvasissati;",
  "tha-ap52:9.3_0": "Pañcasattatikkhattuñca,",
  "tha-ap52:9.4_0": "cakkavattī bhavissati.",
  "tha-ap52:10.1_0": "Dujjayo nāma nāmena,",
  "tha-ap52:10.2_0": "hessati manujādhipo;",
  "tha-ap52:10.3_0": "Anubhotvāna taṁ puññaṁ,",
  "tha-ap52:10.4_0": "sakakammaṁ apassito.",
  "tha-ap52:11.1_0": "Vinipātaṁ agantvāna,",
  "tha-ap52:11.2_0": "manussattaṁ gamissati;",
  "tha-ap52:11.3_0": "Hiraññaṁ tassa nicitaṁ,",
  "tha-ap52:11.4_0": "koṭisatamanappakaṁ.",
  "tha-ap52:12.1_0": "Nibbattissati yonimhi,",
  "tha-ap52:12.2_0": "brāhmaṇe so bhavissati;",
  "tha-ap52:12.3_0": "Vaṅgantassa suto dhīmā,",
  "tha-ap52:12.4_0": "sāriyā oraso piyo.",
  "tha-ap52:13.1_0": "So ca pacchā pabbajitvā,",
  "tha-ap52:13.2_0": "aṅgīrasassa sāsane;",
  "tha-ap52:13.3_0": "Cūḷacundoti nāmena,",
  "tha-ap52:13.4_0": "hessati satthu sāvako.",
  "tha-ap52:14.1_0": "Sāmaṇerova so santo,",
  "tha-ap52:14.2_0": "khīṇāsavo bhavissati;",
  "tha-ap52:14.3_0": "Sabbāsave pariññāya,",
  "tha-ap52:14.4_0": "nibbāyissatināsavo’.",
  "tha-ap52:15.1_0": "Upaṭṭhahiṁ mahāvīraṁ,",
  "tha-ap52:15.2_0": "aññe ca pesale bahū;",
  "tha-ap52:15.3_0": "Bhātaraṁ me cupaṭṭhāsiṁ,",
  "tha-ap52:15.4_0": "uttamatthassa pattiyā.",
  "tha-ap52:16.1_0": "Bhātaraṁ me upaṭṭhitvā,",
  "tha-ap52:16.2_0": "dhātuṁ pattamhi ohiya;",
  "tha-ap52:16.3_0": "Sambuddhaṁ upanāmesiṁ,",
  "tha-ap52:16.4_0": "lokajeṭṭhaṁ narāsabhaṁ.",
  "tha-ap52:17.1_0": "Ubho hatthehi paggayha,",
  "tha-ap52:17.2_0": "buddho loke sadevake;",
  "tha-ap52:17.3_0": "Sandassayanto taṁ dhātuṁ,",
  "tha-ap52:17.4_0": "kittayi aggasāvakaṁ.",
  "tha-ap52:18.1_0": "Cittañca suvimuttaṁ me,",
  "tha-ap52:18.2_0": "saddhā mayhaṁ patiṭṭhitā;",
  "tha-ap52:18.3_0": "Sabbāsave pariññāya,",
  "tha-ap52:18.4_0": "viharāmi anāsavo.",
  "tha-ap52:19.1_0": "Paṭisambhidānuppattā,",
  "tha-ap52:19.2_0": "Vimokkhāpi ca phassitā;",
  "tha-ap52:19.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap52:19.4_0": "Kataṁ buddhassa sāsanaṁ”.",
  "tha-ap52:20.1_0": "Itthaṁ sudaṁ āyasmā cundo thero imā gāthāyo abhāsitthāti.",
  "tha-ap52:21.1_0": "Cundattherassāpadānaṁ dasamaṁ.",
  "tha-ap52:22.1_0": "Upālivaggo pañcamo.",
  "tha-ap52:23.1_0": "Tassuddānaṁ",
  "tha-ap52:24.1_0": "Upāli soṇo bhaddiyo,",
  "tha-ap52:24.2_0": "sanniṭṭhāpakahatthiyo;",
  "tha-ap52:24.3_0": "Chadanaṁ seyyacaṅkamaṁ,",
  "tha-ap52:24.4_0": "subhaddo cundasavhayo;",
  "tha-ap52:24.5_0": "Gāthāsataṁ satālīsaṁ,",
  "tha-ap52:24.6_0": "catasso ca taduttari."
}