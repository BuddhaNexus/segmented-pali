{
  "vv39:0.1_0": "Vimānavatthu",
  "vv39:0.2_0": "Itthivimāna",
  "vv39:0.3_0": "Mañjiṭṭhakavagga",
  "vv39:0.4_0": "1. Mañjiṭṭhakavimānavatthu",
  "vv39:1.1_0": "“Mañjiṭṭhake vimānasmiṁ,",
  "vv39:1.2_0": "soṇṇavālukasanthate;",
  "vv39:1.3_0": "Pañcaṅgikena tūriyena,",
  "vv39:1.4_0": "ramasi suppavādite.",
  "vv39:2.1_0": "Tamhā vimānā oruyha,",
  "vv39:2.2_0": "nimmitā ratanāmayā;",
  "vv39:2.3_0": "Ogāhasi sālavanaṁ,",
  "vv39:2.4_0": "pupphitaṁ sabbakālikaṁ.",
  "vv39:3.1_0": "Yassa yasseva sālassa,",
  "vv39:3.2_0": "mūle tiṭṭhasi devate;",
  "vv39:3.3_0": "So so muñcati pupphāni,",
  "vv39:3.4_0": "onamitvā dumuttamo.",
  "vv39:4.1_0": "Vāteritaṁ sālavanaṁ,",
  "vv39:4.2_0": "ādhutaṁ dijasevitaṁ;",
  "vv39:4.3_0": "Vāti gandho disā sabbā,",
  "vv39:4.4_0": "rukkho mañjūsako yathā.",
  "vv39:5.1_0": "Ghāyase taṁ sucigandhaṁ,",
  "vv39:5.2_0": "rūpaṁ passasi amānusaṁ;",
  "vv39:5.3_0": "Devate pucchitācikkha,",
  "vv39:5.4_0": "kissa kammassidaṁ phalan”ti.",
  "vv39:6.1_0": "“Ahaṁ manussesu manussabhūtā,",
  "vv39:6.2_0": "Dāsī ayirakule ahuṁ;",
  "vv39:6.3_0": "Buddhaṁ nisinnaṁ disvāna,",
  "vv39:6.4_0": "Sālapupphehi okiriṁ.",
  "vv39:7.1_0": "Vaṭaṁsakañca sukataṁ,",
  "vv39:7.2_0": "sālapupphamayaṁ ahaṁ;",
  "vv39:7.3_0": "Buddhassa upanāmesiṁ,",
  "vv39:7.4_0": "pasannā sehi pāṇibhi.",
  "vv39:8.1_0": "Tāhaṁ kammaṁ karitvāna,",
  "vv39:8.2_0": "kusalaṁ buddhavaṇṇitaṁ;",
  "vv39:8.3_0": "Apetasokā sukhitā,",
  "vv39:8.4_0": "sampamodāmanāmayā”ti.",
  "vv39:9.1_0": "Mañjiṭṭhakavimānaṁ paṭhamaṁ."
}