{
  "vv42:0.1_0": "Vimānavatthu",
  "vv42:0.2_0": "Itthivimāna",
  "vv42:0.3_0": "Mañjiṭṭhakavagga",
  "vv42:0.4_0": "4. Alomavimānavatthu",
  "vv42:1.1_0": "“Abhikkantena vaṇṇena,",
  "vv42:1.2_0": "yā tvaṁ tiṭṭhasi devate;",
  "vv42:1.3_0": "Obhāsentī disā sabbā,",
  "vv42:1.4_0": "osadhī viya tārakā.",
  "vv42:2.1_0": "Kena tetādiso vaṇṇo,",
  "vv42:2.2_0": "…pe…",
  "vv42:2.3_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv42:3.1_0": "Sā devatā attamanā,",
  "vv42:3.2_0": "…pe…",
  "vv42:3.3_0": "yassa kammassidaṁ phalaṁ.",
  "vv42:4.1_0": "“Ahañca bārāṇasiyaṁ,",
  "vv42:4.2_0": "buddhassādiccabandhuno;",
  "vv42:4.3_0": "Adāsiṁ sukkhakummāsaṁ,",
  "vv42:4.4_0": "pasannā sehi pāṇibhi.",
  "vv42:5.1_0": "Sukkhāya aloṇikāya ca,",
  "vv42:5.2_0": "Passa phalaṁ kummāsapiṇḍiyā;",
  "vv42:5.3_0": "Alomaṁ sukhitaṁ disvā,",
  "vv42:5.4_0": "Ko puññaṁ na karissati.",
  "vv42:6.1_0": "Tena metādiso vaṇṇo,",
  "vv42:6.2_0": "…pe…",
  "vv42:6.3_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv42:7.1_0": "Alomavimānaṁ catutthaṁ."
}