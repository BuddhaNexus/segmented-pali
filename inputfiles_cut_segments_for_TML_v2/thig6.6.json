{
  "thig6.6:0.1_0": "Therīgāthā",
  "thig6.6:0.2_0": "Chakkanipāta",
  "thig6.6:0.3_0": "6 Mahāpajāpatigotamītherīgāthā",
  "thig6.6:1.1_0": "“Buddha vīra namo tyatthu,",
  "thig6.6:1.2_0": "sabbasattānamuttama;",
  "thig6.6:1.3_0": "Yo maṁ dukkhā pamocesi,",
  "thig6.6:1.4_0": "aññañca bahukaṁ janaṁ.",
  "thig6.6:2.1_0": "Sabbadukkhaṁ pariññātaṁ,",
  "thig6.6:2.2_0": "Hetutaṇhā visositā;",
  "thig6.6:2.3_0": "Bhāvito aṭṭhaṅgiko maggo,",
  "thig6.6:2.4_0": "Nirodho phusito mayā.",
  "thig6.6:3.1_0": "Mātā putto pitā bhātā,",
  "thig6.6:3.2_0": "ayyakā ca pure ahuṁ;",
  "thig6.6:3.3_0": "Yathābhuccamajānantī,",
  "thig6.6:3.4_0": "saṁsariṁhaṁ anibbisaṁ.",
  "thig6.6:4.1_0": "Diṭṭho hi me so bhagavā,",
  "thig6.6:4.2_0": "antimoyaṁ samussayo;",
  "thig6.6:4.3_0": "Vikkhīṇo jātisaṁsāro,",
  "thig6.6:4.4_0": "natthi dāni punabbhavo.",
  "thig6.6:5.1_0": "Āraddhavīriye pahitatte,",
  "thig6.6:5.2_0": "Niccaṁ daḷhaparakkame;",
  "thig6.6:5.3_0": "Samagge sāvake passe,",
  "thig6.6:5.4_0": "Esā buddhāna vandanā.",
  "thig6.6:6.1_0": "Bahūnaṁ vata atthāya,",
  "thig6.6:6.2_0": "Māyā janayi gotamaṁ;",
  "thig6.6:6.3_0": "Byādhimaraṇatunnānaṁ,",
  "thig6.6:6.4_0": "Dukkhakkhandhaṁ byapānudī”ti.",
  "thig6.6:7.1_0": "… Mahāpajāpatigotamī therī …."
}