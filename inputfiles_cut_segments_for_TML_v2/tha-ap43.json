{
  "tha-ap43:0.1_0": "Therāpadāna",
  "tha-ap43:0.2_0": "Upālivagga",
  "tha-ap43:0.3_0": "1 Bhāgineyyupālittheraapadāna",
  "tha-ap43:1.1_0": "“Khīṇāsavasahassehi,",
  "tha-ap43:1.2_0": "parivuto lokanāyako;",
  "tha-ap43:1.3_0": "Vivekamanuyutto so,",
  "tha-ap43:1.4_0": "gacchate paṭisallituṁ.",
  "tha-ap43:2.1_0": "Ajinena nivatthohaṁ,",
  "tha-ap43:2.2_0": "tidaṇḍaparidhārako;",
  "tha-ap43:2.3_0": "Bhikkhusaṅghaparibyūḷhaṁ,",
  "tha-ap43:2.4_0": "addasaṁ lokanāyakaṁ.",
  "tha-ap43:3.1_0": "Ekaṁsaṁ ajinaṁ katvā,",
  "tha-ap43:3.2_0": "sire katvāna añjaliṁ;",
  "tha-ap43:3.3_0": "Sambuddhaṁ abhivādetvā,",
  "tha-ap43:3.4_0": "santhaviṁ lokanāyakaṁ.",
  "tha-ap43:4.1_0": "Yathāṇḍajā ca saṁsedā,",
  "tha-ap43:4.2_0": "opapātī jalābujā;",
  "tha-ap43:4.3_0": "Kākādipakkhino sabbe,",
  "tha-ap43:4.4_0": "antalikkhacarā sadā.",
  "tha-ap43:5.1_0": "‘Ye keci pāṇabhūtatthi,",
  "tha-ap43:5.2_0": "saññino vā asaññino;",
  "tha-ap43:5.3_0": "Sabbe te tava ñāṇamhi,",
  "tha-ap43:5.4_0": "anto honti samogadhā.",
  "tha-ap43:6.1_0": "Gandhā ca pabbateyyā ye,",
  "tha-ap43:6.2_0": "himavantanaguttame;",
  "tha-ap43:6.3_0": "Sabbe te tava sīlamhi,",
  "tha-ap43:6.4_0": "kalāyapi na yujjare.",
  "tha-ap43:7.1_0": "Mohandhakārapakkhando,",
  "tha-ap43:7.2_0": "ayaṁ loko sadevako;",
  "tha-ap43:7.3_0": "Tava ñāṇamhi jotante,",
  "tha-ap43:7.4_0": "andhakārā vidhaṁsitā.",
  "tha-ap43:8.1_0": "Yathā atthaṅgate sūriye,",
  "tha-ap43:8.2_0": "honti sattā tamogatā;",
  "tha-ap43:8.3_0": "Evaṁ buddhe anuppanne,",
  "tha-ap43:8.4_0": "hoti loko tamogato.",
  "tha-ap43:9.1_0": "Yathodayanto ādicco,",
  "tha-ap43:9.2_0": "vinodeti tamaṁ sadā;",
  "tha-ap43:9.3_0": "Tatheva tvaṁ buddhaseṭṭha,",
  "tha-ap43:9.4_0": "viddhaṁsesi tamaṁ sadā.",
  "tha-ap43:10.1_0": "Padhānapahitattosi,",
  "tha-ap43:10.2_0": "buddho loke sadevake;",
  "tha-ap43:10.3_0": "Tava kammābhiraddhena,",
  "tha-ap43:10.4_0": "tosesi janataṁ bahuṁ’.",
  "tha-ap43:11.1_0": "Taṁ sabbaṁ anumoditvā,",
  "tha-ap43:11.2_0": "padumuttaro mahāmuni;",
  "tha-ap43:11.3_0": "Nabhaṁ abbhuggamī dhīro,",
  "tha-ap43:11.4_0": "haṁsarājāva ambare.",
  "tha-ap43:12.1_0": "Abbhuggantvāna sambuddho,",
  "tha-ap43:12.2_0": "mahesi padumuttaro;",
  "tha-ap43:12.3_0": "Antalikkhe ṭhito satthā,",
  "tha-ap43:12.4_0": "imā gāthā abhāsatha.",
  "tha-ap43:13.1_0": "Yenidaṁ thavitaṁ ñāṇaṁ,",
  "tha-ap43:13.2_0": "opammehi samāyutaṁ;",
  "tha-ap43:13.3_0": "Tamahaṁ kittayissāmi,",
  "tha-ap43:13.4_0": "suṇātha mama bhāsato.",
  "tha-ap43:14.1_0": "‘Aṭṭhārasañca khattuṁ so,",
  "tha-ap43:14.2_0": "devarājā bhavissati;",
  "tha-ap43:14.3_0": "Pathabyā rajjaṁ tisataṁ,",
  "tha-ap43:14.4_0": "vasudhaṁ āvasissati.",
  "tha-ap43:15.1_0": "Pañcavīsatikkhattuñca,",
  "tha-ap43:15.2_0": "cakkavattī bhavissati;",
  "tha-ap43:15.3_0": "Padesarajjaṁ vipulaṁ,",
  "tha-ap43:15.4_0": "gaṇanāto asaṅkhiyaṁ.",
  "tha-ap43:16.1_0": "Kappasatasahassamhi,",
  "tha-ap43:16.2_0": "okkākakulasambhavo;",
  "tha-ap43:16.3_0": "Gotamo nāma gottena,",
  "tha-ap43:16.4_0": "satthā loke bhavissati.",
  "tha-ap43:17.1_0": "Tusitā hi cavitvāna,",
  "tha-ap43:17.2_0": "sukkamūlena codito;",
  "tha-ap43:17.3_0": "Hīnova jātiyā santo,",
  "tha-ap43:17.4_0": "upāli nāma hessati.",
  "tha-ap43:18.1_0": "So pacchā pabbajitvāna,",
  "tha-ap43:18.2_0": "virājetvāna pāpakaṁ;",
  "tha-ap43:18.3_0": "Sabbāsave pariññāya,",
  "tha-ap43:18.4_0": "nibbāyissatināsavo.",
  "tha-ap43:19.1_0": "Tuṭṭho ca gotamo buddho,",
  "tha-ap43:19.2_0": "sakyaputto mahāyaso;",
  "tha-ap43:19.3_0": "Vinayādhigataṁ tassa,",
  "tha-ap43:19.4_0": "etadagge ṭhapessati’.",
  "tha-ap43:20.1_0": "Saddhāyāhaṁ pabbajito,",
  "tha-ap43:20.2_0": "katakicco anāsavo;",
  "tha-ap43:20.3_0": "Sabbāsave pariññāya,",
  "tha-ap43:20.4_0": "viharāmi anāsavo.",
  "tha-ap43:21.1_0": "Bhagavā cānukampī maṁ,",
  "tha-ap43:21.2_0": "vinayehaṁ visārado;",
  "tha-ap43:21.3_0": "Sakakammābhiraddho ca,",
  "tha-ap43:21.4_0": "viharāmi anāsavo.",
  "tha-ap43:22.1_0": "Saṁvuto pātimokkhamhi,",
  "tha-ap43:22.2_0": "indriyesu ca pañcasu;",
  "tha-ap43:22.3_0": "Dhāremi vinayaṁ sabbaṁ,",
  "tha-ap43:22.4_0": "kevalaṁ ratanākaraṁ.",
  "tha-ap43:23.1_0": "Mamañca guṇamaññāya,",
  "tha-ap43:23.2_0": "satthā loke anuttaro;",
  "tha-ap43:23.3_0": "Bhikkhusaṅghe nisīditvā,",
  "tha-ap43:23.4_0": "etadagge ṭhapesi maṁ.",
  "tha-ap43:24.1_0": "Paṭisambhidā catasso,",
  "tha-ap43:24.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap43:24.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap43:24.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap43:25.1_0": "Itthaṁ sudaṁ āyasmā upāli thero imā gāthāyo abhāsitthāti.",
  "tha-ap43:26.1_0": "Bhāgineyyupālittherassāpadānaṁ paṭhamaṁ."
}