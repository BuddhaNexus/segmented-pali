{
  "thag3.11:0.1_0": "Theragāthā",
  "thag3.11:0.2_0": "Tikanipāta",
  "thag3.11:0.3_0": "Paṭhamavagga",
  "thag3.11:0.4_0": "3.11. Upālittheragāthā",
  "thag3.11:1.1_0": "“Saddhāya abhinikkhamma,",
  "thag3.11:1.2_0": "navapabbajito navo;",
  "thag3.11:1.3_0": "Mitte bhajeyya kalyāṇe,",
  "thag3.11:1.4_0": "suddhājīve atandite.",
  "thag3.11:2.1_0": "Saddhāya abhinikkhamma,",
  "thag3.11:2.2_0": "navapabbajito navo;",
  "thag3.11:2.3_0": "Saṅghasmiṁ viharaṁ bhikkhu,",
  "thag3.11:2.4_0": "sikkhetha vinayaṁ budho.",
  "thag3.11:3.1_0": "Saddhāya abhinikkhamma,",
  "thag3.11:3.2_0": "navapabbajito navo;",
  "thag3.11:3.3_0": "Kappākappesu kusalo,",
  "thag3.11:3.4_0": "careyya apurakkhato”ti.",
  "thag3.11:3.5_0": "… Upālitthero …."
}