{
  "ja272:0.1_0": "Jātaka",
  "ja272:0.2_0": "Tikanipāta",
  "ja272:0.3_0": "Udapānavagga",
  "ja272:0.4_0": "2. Byagghajātaka",
  "ja272:1.1_0": "“Yena mittena saṁsaggā,",
  "ja272:1.2_0": "yogakkhemo vihiyyati;",
  "ja272:1.3_0": "Pubbevajjhābhavaṁ tassa,",
  "ja272:1.4_0": "rukkhe akkhīva paṇḍito.",
  "ja272:2.1_0": "Yena mittena saṁsaggā,",
  "ja272:2.2_0": "yogakkhemo pavaḍḍhati;",
  "ja272:2.3_0": "Kareyyattasamaṁ vuttiṁ,",
  "ja272:2.4_0": "sabbakiccesu paṇḍito”.",
  "ja272:3.1_0": "“Etha byagghā nivattavho,",
  "ja272:3.2_0": "Paccupetha mahāvanaṁ;",
  "ja272:3.3_0": "Mā vanaṁ chindi nibyagghaṁ,",
  "ja272:3.4_0": "Byagghā māhesu nibbanā”ti.",
  "ja272:4.1_0": "Byagghajātakaṁ dutiyaṁ."
}