{
  "iti3:0.1_0": "Itivuttaka 3",
  "iti3:0.2_0": "Ekakanipāta",
  "iti3:0.3_0": "Paṭhamavagga",
  "iti3:0.4_0": "Mohasutta",
  "iti3:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti3:2.1_0": "“Ekadhammaṁ, bhikkhave, pajahatha;",
  "iti3:2.2_0": "ahaṁ vo pāṭibhogo anāgāmitāya.",
  "iti3:2.3_0": "Katamaṁ ekadhammaṁ?",
  "iti3:2.4_0": "Mohaṁ, bhikkhave, ekadhammaṁ pajahatha;",
  "iti3:2.5_0": "ahaṁ vo pāṭibhogo anāgāmitāyā”ti.",
  "iti3:2.6_0": "Etamatthaṁ bhagavā avoca.",
  "iti3:2.7_0": "Tatthetaṁ iti vuccati:",
  "iti3:3.1_0": "“Yena mohena mūḷhāse,",
  "iti3:3.2_0": "sattā gacchanti duggatiṁ;",
  "iti3:3.3_0": "Taṁ mohaṁ sammadaññāya,",
  "iti3:3.4_0": "pajahanti vipassino;",
  "iti3:3.5_0": "Pahāya na punāyanti,",
  "iti3:3.6_0": "imaṁ lokaṁ kudācanan”ti.",
  "iti3:4.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti3:4.2_0": "Tatiyaṁ."
}