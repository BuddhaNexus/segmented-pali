{
  "tha-ap441:0.1_0": "Therāpadāna",
  "tha-ap441:0.2_0": "Ekavihārivagga",
  "tha-ap441:0.3_0": "9. Ambapiṇḍiyattheraapadāna",
  "tha-ap441:1.1_0": "“Hatthirājā tadā āsiṁ,",
  "tha-ap441:1.2_0": "īsādanto uruḷhavā;",
  "tha-ap441:1.3_0": "Vicaranto brahāraññe,",
  "tha-ap441:1.4_0": "addasaṁ lokanāyakaṁ.",
  "tha-ap441:2.1_0": "Ambapiṇḍaṁ gahetvāna,",
  "tha-ap441:2.2_0": "adāsiṁ satthuno ahaṁ;",
  "tha-ap441:2.3_0": "Paṭiggaṇhi mahāvīro,",
  "tha-ap441:2.4_0": "siddhattho lokanāyako.",
  "tha-ap441:3.1_0": "Mama nijjhāyamānassa,",
  "tha-ap441:3.2_0": "paribhuñji tadā jino;",
  "tha-ap441:3.3_0": "Tattha cittaṁ pasādetvā,",
  "tha-ap441:3.4_0": "tusitaṁ upapajjahaṁ.",
  "tha-ap441:4.1_0": "Tato ahaṁ cavitvāna,",
  "tha-ap441:4.2_0": "cakkavattī ahosahaṁ;",
  "tha-ap441:4.3_0": "Eteneva upāyena,",
  "tha-ap441:4.4_0": "anubhutvāna sampadā.",
  "tha-ap441:5.1_0": "Padhānapahitattohaṁ,",
  "tha-ap441:5.2_0": "upasanto nirūpadhi;",
  "tha-ap441:5.3_0": "Sabbāsave pariññāya,",
  "tha-ap441:5.4_0": "viharāmi anāsavo.",
  "tha-ap441:6.1_0": "Catunnavutito kappe,",
  "tha-ap441:6.2_0": "yaṁ phalamadadiṁ tadā;",
  "tha-ap441:6.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap441:6.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap441:7.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap441:7.2_0": "…pe…",
  "tha-ap441:7.3_0": "viharāmi anāsavo.",
  "tha-ap441:8.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap441:8.2_0": "…pe…",
  "tha-ap441:8.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap441:9.1_0": "Paṭisambhidā catasso,",
  "tha-ap441:9.2_0": "…pe…",
  "tha-ap441:9.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap441:10.1_0": "Itthaṁ sudaṁ āyasmā ambapiṇḍiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap441:11.1_0": "Ambapiṇḍiyattherassāpadānaṁ navamaṁ."
}