{
  "ja313:0.1_0": "Jātaka",
  "ja313:0.2_0": "Catukkanipāta",
  "ja313:0.3_0": "Pucimandavagga",
  "ja313:0.4_0": "3. Khantīvādījātaka",
  "ja313:1.1_0": "“Yo te hatthe ca pāde ca,",
  "ja313:1.2_0": "kaṇṇanāsañca chedayi;",
  "ja313:1.3_0": "Tassa kujjha mahāvīra,",
  "ja313:1.4_0": "mā raṭṭhaṁ vinasā idaṁ”.",
  "ja313:2.1_0": "“Yo me hatthe ca pāde ca,",
  "ja313:2.2_0": "kaṇṇanāsañca chedayi;",
  "ja313:2.3_0": "Ciraṁ jīvatu so rājā,",
  "ja313:2.4_0": "na hi kujjhanti mādisā”.",
  "ja313:3.1_0": "Ahū atītamaddhānaṁ,",
  "ja313:3.2_0": "samaṇo khantidīpano;",
  "ja313:3.3_0": "Taṁ khantiyāyeva ṭhitaṁ,",
  "ja313:3.4_0": "kāsirājā achedayi.",
  "ja313:4.1_0": "Tassa kammapharusassa,",
  "ja313:4.2_0": "vipāko kaṭuko ahu;",
  "ja313:4.3_0": "Yaṁ kāsirājā vedesi,",
  "ja313:4.4_0": "nirayamhi samappitoti.",
  "ja313:5.1_0": "Khantīvādījātakaṁ tatiyaṁ."
}