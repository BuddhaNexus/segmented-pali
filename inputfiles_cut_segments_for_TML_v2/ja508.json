{
  "ja508:0.1_0": "Jātaka",
  "ja508:0.2_0": "Vīsatinipāta",
  "ja508:0.3_0": "Mātaṅgavagga",
  "ja508:0.4_0": "12. Pañcapaṇḍitajātaka",
  "ja508:1.1_0": "“Pañca paṇḍitā samāgatāttha,",
  "ja508:1.2_0": "Pañhā me paṭibhāti taṁ suṇātha;",
  "ja508:1.3_0": "Nindiyamatthaṁ pasaṁsiyaṁ vā,",
  "ja508:1.4_0": "Kassevāvikareyya guyhamatthaṁ”.",
  "ja508:2.1_0": "“Tvaṁ āvikarohi bhūmipāla,",
  "ja508:2.2_0": "Bhattā bhārasaho tuvaṁ vade taṁ;",
  "ja508:2.3_0": "Tava chandarucīni sammasitvā,",
  "ja508:2.4_0": "Atha vakkhanti janinda pañca dhīrā”.",
  "ja508:3.1_0": "“Yā sīlavatī anaññatheyyā,",
  "ja508:3.2_0": "Bhattucchandavasānugā manāpā;",
  "ja508:3.3_0": "Nindiyamatthaṁ pasaṁsiyaṁ vā,",
  "ja508:3.4_0": "Bhariyāyāvikareyya guyhamatthaṁ”.",
  "ja508:4.1_0": "“Yo kicchagatassa āturassa,",
  "ja508:4.2_0": "Saraṇaṁ hoti gatī parāyanañca;",
  "ja508:4.3_0": "Nindiyamatthaṁ pasaṁsiyaṁ vā,",
  "ja508:4.4_0": "Sakhino vāvikareyya guyhamatthaṁ”.",
  "ja508:5.1_0": "“Jeṭṭho atha majjhimo kaniṭṭho,",
  "ja508:5.2_0": "Yo ce sīlasamāhito ṭhitatto;",
  "ja508:5.3_0": "Nindiyamatthaṁ pasaṁsiyaṁ vā,",
  "ja508:5.4_0": "Bhātu vāvīkareyya guyhamatthaṁ”.",
  "ja508:6.1_0": "“Yo ve pituhadayassa paddhagū,",
  "ja508:6.2_0": "Anujāto pitaraṁ anomapañño;",
  "ja508:6.3_0": "Nindiyamatthaṁ pasaṁsiyaṁ vā,",
  "ja508:6.4_0": "Puttassāvikareyya guyhamatthaṁ”.",
  "ja508:7.1_0": "“Mātā dvipadājanindaseṭṭha,",
  "ja508:7.2_0": "Yā naṁ poseti chandasā piyena;",
  "ja508:7.3_0": "Nindiyamatthaṁ pasaṁsiyaṁ vā,",
  "ja508:7.4_0": "Mātuyāvīkareyya guyhamatthaṁ”.",
  "ja508:8.1_0": "“Guyhassa hi guyhameva sādhu,",
  "ja508:8.2_0": "Na hi guyhassa pasatthamāvikammaṁ;",
  "ja508:8.3_0": "Anipphannatā saheyya dhīro,",
  "ja508:8.4_0": "Nipphannova yathāsukhaṁ bhaṇeyya”.",
  "ja508:9.1_0": "“Kiṁ tvaṁ vimanosi rājaseṭṭha,",
  "ja508:9.2_0": "Dvipadajaninda vacanaṁ suṇoma metaṁ;",
  "ja508:9.3_0": "Kiṁ cintayamāno dummanosi,",
  "ja508:9.4_0": "Nūna deva aparādho atthi mayhaṁ”.",
  "ja508:10.1_0": "“‘Paṇhe vajjho mahosadho’ti,",
  "ja508:10.2_0": "Āṇatto me vadhāya bhūripañño;",
  "ja508:10.3_0": "Taṁ cintayamāno dummanosmi,",
  "ja508:10.4_0": "Na hi devī aparādho atthi tuyhaṁ”.",
  "ja508:11.1_0": "“Abhidosagato dāni ehisi,",
  "ja508:11.2_0": "Kiṁ sutvā kiṁ saṅkate mano te;",
  "ja508:11.3_0": "Ko te kimavoca bhūripañña,",
  "ja508:11.4_0": "Iṅgha vacanaṁ suṇoma brūhi metaṁ”.",
  "ja508:12.1_0": "“‘Paṇhe vajjho mahosadho’ti,",
  "ja508:12.2_0": "Yadi te mantayitaṁ janinda dosaṁ;",
  "ja508:12.3_0": "Bhariyāya rahogato asaṁsi,",
  "ja508:12.4_0": "Guyhaṁ pātukataṁ sutaṁ mametaṁ”.",
  "ja508:13.1_0": "“Yaṁ sālavanasmiṁ senako,",
  "ja508:13.2_0": "Pāpakammaṁ akāsi asabbhirūpaṁ;",
  "ja508:13.3_0": "Sakhinova rahogato asaṁsi,",
  "ja508:13.4_0": "Guyhaṁ pātukataṁ sutaṁ mametaṁ.",
  "ja508:14.1_0": "Pukkusa purisassa te janinda,",
  "ja508:14.2_0": "Uppanno rogo arājayutto;",
  "ja508:14.3_0": "Bhātuñca rahogato asaṁsi,",
  "ja508:14.4_0": "Guyhaṁ pātukataṁ sutaṁ mametaṁ.",
  "ja508:15.1_0": "Ābādhoyaṁ asabbhirūpo,",
  "ja508:15.2_0": "Kāmindo naradevena phuṭṭho;",
  "ja508:15.3_0": "Puttassa rahogato asaṁsi,",
  "ja508:15.4_0": "Guyhaṁ pātukataṁ sutaṁ mametaṁ.",
  "ja508:16.1_0": "Aṭṭhavaṅkaṁ maṇiratanaṁ uḷāraṁ,",
  "ja508:16.2_0": "Sakko te adadā pitāmahassa;",
  "ja508:16.3_0": "Devindassa gataṁ tadajja hatthaṁ,",
  "ja508:16.4_0": "Mātuñca rahogato asaṁsi;",
  "ja508:16.5_0": "Guyhaṁ pātukataṁ sutaṁ mametaṁ.",
  "ja508:17.1_0": "Guyhassa hi guyhameva sādhu,",
  "ja508:17.2_0": "Na hi guyhassa pasatthamāvikammaṁ;",
  "ja508:17.3_0": "Anipphannatā saheyya dhīro,",
  "ja508:17.4_0": "Nipphannova yathāsukhaṁ bhaṇeyya.",
  "ja508:18.1_0": "Na guyhamatthaṁ vivareyya,",
  "ja508:18.2_0": "rakkheyya naṁ yathā nidhiṁ;",
  "ja508:18.3_0": "Na hi pātukato sādhu,",
  "ja508:18.4_0": "guyho attho pajānatā.",
  "ja508:19.1_0": "Thiyā guyhaṁ na saṁseyya,",
  "ja508:19.2_0": "amittassa ca paṇḍito;",
  "ja508:19.3_0": "Yo cāmisena saṁhīro,",
  "ja508:19.4_0": "hadayattheno ca yo naro.",
  "ja508:20.1_0": "Guyhamatthaṁ asambuddhaṁ,",
  "ja508:20.2_0": "sambodhayati yo naro;",
  "ja508:20.3_0": "Mantabhedabhayā tassa,",
  "ja508:20.4_0": "dāsabhūto titikkhati.",
  "ja508:21.1_0": "Yāvanto purisassatthaṁ,",
  "ja508:21.2_0": "guyhaṁ jānanti mantinaṁ;",
  "ja508:21.3_0": "Tāvanto tassa ubbegā,",
  "ja508:21.4_0": "tasmā guyhaṁ na vissaje.",
  "ja508:22.1_0": "Vivicca bhāseyya divā rahassaṁ,",
  "ja508:22.2_0": "Rattiṁ giraṁ nātivelaṁ pamuñce;",
  "ja508:22.3_0": "Upassutikā hi suṇanti mantaṁ,",
  "ja508:22.4_0": "Tasmā manto khippamupeti bhedan”ti.",
  "ja508:23.1_0": "Pañcapaṇḍitajātakaṁ dvādasamaṁ."
}