{
  "iti34:0.1_0": "Itivuttaka 34",
  "iti34:0.2_0": "Dukanipāta",
  "iti34:0.3_0": "Paṭhamavagga",
  "iti34:0.4_0": "Ātāpīsutta",
  "iti34:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti34:2.1_0": "“Anātāpī, bhikkhave, bhikkhu anottāpī abhabbo sambodhāya, abhabbo nibbānāya, abhabbo anuttarassa yogakkhemassa adhigamāya.",
  "iti34:2.2_0": "Ātāpī ca kho, bhikkhave, bhikkhu ottāpī bhabbo sambodhāya, bhabbo nibbānāya, bhabbo anuttarassa yogakkhemassa adhigamāyā”ti.",
  "iti34:2.3_0": "Etamatthaṁ bhagavā avoca.",
  "iti34:2.4_0": "Tatthetaṁ iti vuccati:",
  "iti34:3.1_0": "“Anātāpī anottāpī,",
  "iti34:3.2_0": "kusīto hīnavīriyo;",
  "iti34:3.3_0": "Yo thinamiddhabahulo,",
  "iti34:3.4_0": "ahirīko anādaro;",
  "iti34:3.5_0": "Abhabbo tādiso bhikkhu,",
  "iti34:3.6_0": "phuṭṭhuṁ sambodhimuttamaṁ.",
  "iti34:4.1_0": "Yo ca satimā nipako jhāyī,",
  "iti34:4.2_0": "Ātāpī ottāpī ca appamatto;",
  "iti34:4.3_0": "Saṁyojanaṁ jātijarāya chetvā,",
  "iti34:4.4_0": "Idheva sambodhimanuttaraṁ phuse”ti.",
  "iti34:5.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti34:5.2_0": "Sattamaṁ."
}