{
  "anya-e1004n1:0": "Sambhedakathā",
  "anya-e1004n1:1": "81.",
  "anya-e1004n1:2": "Sattha kappavicārena,",
  "anya-e1004n1:3": "Kālo gacchati dhīmataṁ;",
  "anya-e1004n1:4": "Byasanena asādhūnaṁ,",
  "anya-e1004n1:5": "Niddāya kalahenavā.",
  "anya-e1004n1:6": "82.",
  "anya-e1004n1:7": "Sokaṭhānasahassāni,",
  "anya-e1004n1:8": "Bhayaṭhānasatānica;",
  "anya-e1004n1:9": "Divasedivase muḷhaṁ,",
  "anya-e1004n1:10": "Āvīsanti napaṇḍitaṁ.",
  "anya-e1004n1:11": "83.",
  "anya-e1004n1:12": "Atidīgho mahāmuḷho,",
  "anya-e1004n1:13": "Majjhimoca vicakkhaṇo;",
  "anya-e1004n1:14": "Vāsudevaṁ purakkhitvā,",
  "anya-e1004n1:15": "Sabbevāmanakā saṭṭhā.",
  "anya-e1004n1:16": "84.",
  "anya-e1004n1:17": "Na loke sobhate muḷho,",
  "anya-e1004n1:18": "Kevalattapasaṁsako;",
  "anya-e1004n1:19": "Api sampihite kūpe,",
  "anya-e1004n1:20": "Katavijjo pakāsate.",
  "anya-e1004n1:21": "85.",
  "anya-e1004n1:22": "Madantadamanaṁ satthaṁ,",
  "anya-e1004n1:23": "Khalānaṁ kurute madaṁ;",
  "anya-e1004n1:24": "Cakkhusaṅkhārakaṁ tejaṁ,",
  "anya-e1004n1:25": "Uluṅkāna mivandhakaṁ.",
  "anya-e1004n1:26": "86.",
  "anya-e1004n1:27": "Atyappamapi sādhūnaṁ,",
  "anya-e1004n1:28": "Silālekheva tiṭṭhate;",
  "anya-e1004n1:29": "Jalelekheva nīcānaṁ,",
  "anya-e1004n1:30": "Yaṁ kataṁ taṁpi nassati.",
  "anya-e1004n1:31": "87.",
  "anya-e1004n1:32": "Dabbamappaṁpi sādhūnaṁ,",
  "anya-e1004n1:33": "Jalaṁ kūpeva nissayo;",
  "anya-e1004n1:34": "Bahuttaṁpi asādhūnaṁ,",
  "anya-e1004n1:35": "Naca vāriva aṇṇave.",
  "anya-e1004n1:36": "88.",
  "anya-e1004n1:37": "Duṭṭhacitto panāhissa,",
  "anya-e1004n1:38": "Kodho pāsāṇalikkhito;",
  "anya-e1004n1:39": "Kujjhitabbe sujanassa,",
  "anya-e1004n1:40": "Jalelekhā ciraṭṭhito.",
  "anya-e1004n1:41": "89.",
  "anya-e1004n1:42": "Bālassa jīvitaṁ pāpaṁ,",
  "anya-e1004n1:43": "Mitarassi taraṁ bhave;",
  "anya-e1004n1:44": "Janakāyassa rājāva,",
  "anya-e1004n1:45": "Rājadhammova rājunaṁ.",
  "anya-e1004n1:46": "90.",
  "anya-e1004n1:47": "Sappādānaṁ balī sīho,",
  "anya-e1004n1:48": "Puḷāvako tato kippīlikā,",
  "anya-e1004n1:49": "Narā tato balī rājā,",
  "anya-e1004n1:50": "Sabbesa mantako balī.",
  "anya-e1004n1:51": "91.",
  "anya-e1004n1:52": "Bhūpoṇṇava ggi thī sippi,",
  "anya-e1004n1:53": "Abhijjhālucapuggalo;",
  "anya-e1004n1:54": "Etesaṁpi mahicchānaṁ,",
  "anya-e1004n1:55": "Mahicchitā aniccatā.",
  "anya-e1004n1:56": "92.",
  "anya-e1004n1:57": "Niddāluko asantuṭṭho,",
  "anya-e1004n1:58": "Akataññūca bhīruko;",
  "anya-e1004n1:59": "Sakkuṇantina sācāraṁ,",
  "anya-e1004n1:60": "Sikkhituṁ te kadācipi.",
  "anya-e1004n1:61": "93.",
  "anya-e1004n1:62": "Niddālu kāmarāmoca,",
  "anya-e1004n1:63": "Sukhito bhogavālaso;",
  "anya-e1004n1:64": "Nicchanto kammarāmo ca,",
  "anya-e1004n1:65": "Sattete satthavajjitā.",
  "anya-e1004n1:66": "94.",
  "anya-e1004n1:67": "Samiddho dhanadhaññena,",
  "anya-e1004n1:68": "Kaṭṭho dakatiṇaggihi;",
  "anya-e1004n1:69": "Sabbato duggatenaṭṭho,",
  "anya-e1004n1:70": "Tasmā nadukkataṁ kare.",
  "anya-e1004n1:71": "95.",
  "anya-e1004n1:72": "Puññapāpaphalaṁ yo ce,",
  "anya-e1004n1:73": "Nasaddhahati saccato;",
  "anya-e1004n1:74": "So ve khippaṁva attānaṁ,",
  "anya-e1004n1:75": "Ādāsatalamānaye.",
  "anya-e1004n1:76": "96.",
  "anya-e1004n1:77": "Samparāyikamatthaṁ yo,",
  "anya-e1004n1:78": "Nasaddhahati cepiso;",
  "anya-e1004n1:79": "Āvāse sagga gāmīnaṁ,",
  "anya-e1004n1:80": "Mākkabhe kiṁnapassati.",
  "anya-e1004n1:81": "97.",
  "anya-e1004n1:82": "Mahantaṁ vaṭṭarukkhādiṁ,",
  "anya-e1004n1:83": "Khuddabījaṁ bahūphalaṁ;",
  "anya-e1004n1:84": "Sakkhikatvā udikkheyya,",
  "anya-e1004n1:85": "Puññapāpakaro naro.",
  "anya-e1004n1:86": "98.",
  "anya-e1004n1:87": "Yassa sallahukaṁ hoti,",
  "anya-e1004n1:88": "Gurusakkāramānanaṁ;",
  "anya-e1004n1:89": "Tassa sallahukāyeva,",
  "anya-e1004n1:90": "Vijjāsampattisampadā.",
  "anya-e1004n1:91": "99.",
  "anya-e1004n1:92": "Upajjhācariyānañca,",
  "anya-e1004n1:93": "Mātāpitūnamevaca;",
  "anya-e1004n1:94": "Sakkaccaṁ yonupaṭṭhāti,",
  "anya-e1004n1:95": "Suto tassapitādiso.",
  "anya-e1004n1:96": "100.",
  "anya-e1004n1:97": "Dese dese kulānīca,",
  "anya-e1004n1:98": "Desedese ca bandhavo;",
  "anya-e1004n1:99": "Tādisaṁ sahajā yattha,",
  "anya-e1004n1:100": "Desaṁ passāminevatu.",
  "anya-e1004n1:101": "101.",
  "anya-e1004n1:102": "Puttaṁvā bhātaraṁ duṭṭhaṁ,",
  "anya-e1004n1:103": "Anusāseyya nojahe;",
  "anya-e1004n1:104": "Kiṁnu chejjaṁ karaṁpādaṁ,",
  "anya-e1004n1:105": "Littaṁ asucinā siyā;",
  "anya-e1004n1:106": "102.",
  "anya-e1004n1:107": "Bahuputte pitāeko,",
  "anya-e1004n1:108": "Avassaṁ poseti sadā;",
  "anya-e1004n1:109": "Bahuputtā nasakkonti,",
  "anya-e1004n1:110": "Posetuṁ pitarekakaṁ.",
  "anya-e1004n1:111": "103.",
  "anya-e1004n1:112": "Atijātamanujātaṁ,",
  "anya-e1004n1:113": "Puttamicchanti paṇḍitā;",
  "anya-e1004n1:114": "Avajātaṁ naicchanti,",
  "anya-e1004n1:115": "Yohoti kulagandhano.",
  "anya-e1004n1:116": "104.",
  "anya-e1004n1:117": "Pañcaṭhānāni sampassaṁ,",
  "anya-e1004n1:118": "Puttamicchanti paṇḍitā;",
  "anya-e1004n1:119": "Jātovā no bharissati,",
  "anya-e1004n1:120": "Kiccaṁvā no karissati.",
  "anya-e1004n1:121": "105.",
  "anya-e1004n1:122": "Kulavaṁso ciraṁ tiṭṭhe,",
  "anya-e1004n1:123": "Dāyajjaṁ paṭipaccati;",
  "anya-e1004n1:124": "Athavāpana petānaṁ,",
  "anya-e1004n1:125": "Dakkhiṇaṁnupadassati.",
  "anya-e1004n1:126": "106.",
  "anya-e1004n1:127": "Antojāto dhanakkīto,",
  "anya-e1004n1:128": "Dāsabyopagato sayaṁ;",
  "anya-e1004n1:129": "Dāsākaramarānīto,",
  "anya-e1004n1:130": "Ccevante catudhā siyuṁ.",
  "anya-e1004n1:131": "107.",
  "anya-e1004n1:132": "Dāsā pañceva corayya-",
  "anya-e1004n1:133": "Sakhañātyattasadisā;",
  "anya-e1004n1:134": "Tathā viññūhi viññeyyā,",
  "anya-e1004n1:135": "Mittādārāca bandhavā.",
  "anya-e1004n1:136": "108.",
  "anya-e1004n1:137": "Tayova paṇḍitā satthe,",
  "anya-e1004n1:138": "Ahamevātivādīca,",
  "anya-e1004n1:139": "Ahamapiti vādīca,",
  "anya-e1004n1:140": "Nāhantica imetayo.",
  "anya-e1004n1:141": "109.",
  "anya-e1004n1:142": "Vinā satthaṁ nagaccheyya,",
  "anya-e1004n1:143": "Sūro saṅkāmabhūmiyaṁ;",
  "anya-e1004n1:144": "Paṇḍitaddhagū vāṇijjo,",
  "anya-e1004n1:145": "Videsagamano tathā.",
  "anya-e1004n1:146": "110.",
  "anya-e1004n1:147": "Sammā upaparikkhitvā,",
  "anya-e1004n1:148": "Akkharesu padesuca;",
  "anya-e1004n1:149": "Coraghāto siyā sisso,",
  "anya-e1004n1:150": "Guru coraṭṭakārako.",
  "anya-e1004n1:151": "111.",
  "anya-e1004n1:152": "Sādhuttaṁ sujanasamāgamā khalānaṁ,",
  "anya-e1004n1:153": "Sādhūnaṁ nakhalasamāgamā khalattaṁ;",
  "anya-e1004n1:154": "Āmodaṁkusumabhavaṁdadhātibhūmi,",
  "anya-e1004n1:155": "Bhūgandhaṁ naca kusumāni dhārayanti.",
  "anya-e1004n1:156": "112.",
  "anya-e1004n1:157": "Saṭṭhena mittaṁ kalusena dhammaṁ,",
  "anya-e1004n1:158": "Paropatāpena samiddhibhāvaṁ;",
  "anya-e1004n1:159": "Sukhena vijjaṁ pharusena nāriṁ,",
  "anya-e1004n1:160": "Icchanti ye te navapaṇḍitāva."
}