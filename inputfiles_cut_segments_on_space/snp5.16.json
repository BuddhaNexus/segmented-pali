{
  "snp5.16:0.1": "Sutta Nipāta 5.16 ",
  "snp5.16:0.2": "Mogharājamāṇavapucchā ",
  "snp5.16:1.1": "“Dvāhaṁ sakkaṁ apucchissaṁ, ",
  "snp5.16:1.2": "(iccāyasmā mogharājā) ",
  "snp5.16:1.3": "Na me byākāsi cakkhumā; ",
  "snp5.16:1.4": "Yāvatatiyañca devīsi, ",
  "snp5.16:1.5": "Byākarotīti me sutaṁ. ",
  "snp5.16:2.1": "Ayaṁ loko paro loko, ",
  "snp5.16:2.2": "brahmaloko sadevako; ",
  "snp5.16:2.3": "Diṭṭhiṁ te nābhijānāti, ",
  "snp5.16:2.4": "gotamassa yasassino. ",
  "snp5.16:3.1": "Evaṁ abhikkantadassāviṁ, ",
  "snp5.16:3.2": "atthi pañhena āgamaṁ; ",
  "snp5.16:3.3": "Kathaṁ lokaṁ avekkhantaṁ, ",
  "snp5.16:3.4": "maccurājā na passati”. ",
  "snp5.16:4.1": "“Suññato lokaṁ avekkhassu, ",
  "snp5.16:4.2": "Mogharāja sadā sato; ",
  "snp5.16:4.3": "Attānudiṭṭhiṁ ūhacca, ",
  "snp5.16:4.4": "Evaṁ maccutaro siyā; ",
  "snp5.16:4.5": "Evaṁ lokaṁ avekkhantaṁ, ",
  "snp5.16:4.6": "Maccurājā na passatī”ti. ",
  "snp5.16:4.7": "Mogharājamāṇavapucchā pannarasamā. "
}