{
  "pv46:0.1": "Petavatthu ",
  "pv46:0.2": "Mahāvagga ",
  "pv46:0.3": "11. Pāṭaliputtapetavatthu ",
  "pv46:1.1": "“Diṭṭhā tayā nirayā tiracchānayoni, ",
  "pv46:1.2": "Petā asurā athavāpi mānusā devā; ",
  "pv46:1.3": "Sayamaddasa kammavipākamattano, ",
  "pv46:1.4": "Nessāmi taṁ pāṭaliputtamakkhataṁ; ",
  "pv46:1.5": "Tattha gantvā kusalaṁ karohi kammaṁ”. ",
  "pv46:2.1": "“Atthakāmosi me yakkha, ",
  "pv46:2.2": "hitakāmosi devate; ",
  "pv46:2.3": "Karomi tuyhaṁ vacanaṁ, ",
  "pv46:2.4": "tvaṁsi ācariyo mama. ",
  "pv46:3.1": "Diṭṭhā mayā nirayā tiracchānayoni, ",
  "pv46:3.2": "Petā asurā athavāpi mānusā devā; ",
  "pv46:3.3": "Sayamaddasaṁ kammavipākamattano, ",
  "pv46:3.4": "Kāhāmi puññāni anappakānī”ti. ",
  "pv46:4.1": "Pāṭaliputtapetavatthu ekādasamaṁ. "
}