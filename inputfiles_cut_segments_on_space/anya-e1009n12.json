{
  "anya-e1009n12:0": "Sīla-niddesa",
  "anya-e1009n12:1": "291.",
  "anya-e1009n12:2": " Pamādaṁ bhayato disvā,",
  "anya-e1009n12:3": "A-ppamādañca khemato;",
  "anya-e1009n12:4": "Bhāvetha aṭṭhaṅgikaṁ maggaṁ,",
  "anya-e1009n12:5": "Esā buddhānusāsanī.",
  "anya-e1009n12:6": "292.",
  "anya-e1009n12:7": " Hīnena brahma-cariyena,",
  "anya-e1009n12:8": "Khattiye upapajjati;",
  "anya-e1009n12:9": "Majjhimena ca devattaṁ,",
  "anya-e1009n12:10": "Uttamena vi-sujjhati.",
  "anya-e1009n12:11": "Ka.",
  "anya-e1009n12:12": " Nagare bandhumatiyā,",
  "anya-e1009n12:13": "Bandhumā nāma khattiyo;",
  "anya-e1009n12:14": "Divase puṇṇamāya so,",
  "anya-e1009n12:15": "Upagacchi uposathaṁ.",
  "anya-e1009n12:16": "Kha.",
  "anya-e1009n12:17": " Ahaṁ tena samayena,",
  "anya-e1009n12:18": "Gumbha-dāsī ahaṁ tahiṁ;",
  "anya-e1009n12:19": "Disvā sa-rājakaṁ senaṁ,",
  "anya-e1009n12:20": "Evāhaṁ cintayiṁ tadā.",
  "anya-e1009n12:21": "Ga.",
  "anya-e1009n12:22": " Rājāpi rajjaṁ chaṭṭetvā,",
  "anya-e1009n12:23": "Upagacchi uposathaṁ;",
  "anya-e1009n12:24": "Sa-phalaṁ nūna taṁ kammaṁ,",
  "anya-e1009n12:25": "Jana-kāyo pamodito.",
  "anya-e1009n12:26": "Gha.",
  "anya-e1009n12:27": " Yoniso paccavekkhitvā,",
  "anya-e1009n12:28": "Duggaccañca daliddataṁ;",
  "anya-e1009n12:29": "Mānasaṁ sampahaṁsitvā,",
  "anya-e1009n12:30": "Upagacchimu posathaṁ.",
  "anya-e1009n12:31": "Ṅa.",
  "anya-e1009n12:32": " Ahaṁ uposathaṁ katvā,",
  "anya-e1009n12:33": "Sammā-sambuddhasāsane;",
  "anya-e1009n12:34": "Tena kammena su-katena,",
  "anya-e1009n12:35": "Tāvatiṁsaṁ agacchahaṁ.",
  "anya-e1009n12:36": "Ca.",
  "anya-e1009n12:37": " Tattha me su-kataṁ byamhaṁ,",
  "anya-e1009n12:38": "Ubbha-yojanamuggataṁ;",
  "anya-e1009n12:39": "Kūṭāgāra-varūpetaṁ,",
  "anya-e1009n12:40": "Mahāsanasu-bhūsitaṁ.",
  "anya-e1009n12:41": "Cha.",
  "anya-e1009n12:42": " Accharā sata-sahassā,",
  "anya-e1009n12:43": "Upatiṭṭhanti maṁ sadā;",
  "anya-e1009n12:44": "Aññe deve atikkamma,",
  "anya-e1009n12:45": "Atirocāmi sabbadā.",
  "anya-e1009n12:46": "Ja.",
  "anya-e1009n12:47": " Catusaṭṭhi-deva-rājūnaṁ ,",
  "anya-e1009n12:48": "Mahesittamakārayiṁ;",
  "anya-e1009n12:49": "Tesaṭṭhi-cakkavattinaṁ,",
  "anya-e1009n12:50": "Mahesittamakārayiṁ.",
  "anya-e1009n12:51": "Jha.",
  "anya-e1009n12:52": " Suvaṇṇa-vaṇā hutvāna,",
  "anya-e1009n12:53": "Bhavesu saṁsarāmahaṁ;",
  "anya-e1009n12:54": "Sabbattha pavarā homi,",
  "anya-e1009n12:55": "Uposathassidaṁ phalaṁ.",
  "anya-e1009n12:56": "Ña.",
  "anya-e1009n12:57": " Hatthi-yānaṁ assa-yānaṁ,",
  "anya-e1009n12:58": "Ratha-yānañca sivikaṁ;",
  "anya-e1009n12:59": "Labhāmi sabbametampi,",
  "anya-e1009n12:60": "Uposathassidaṁ phalaṁ.",
  "anya-e1009n12:61": "Ṭa.",
  "anya-e1009n12:62": " Soṇṇa-mayaṁ rūpi-mayaṁ,",
  "anya-e1009n12:63": "Athopi phalikā-mayaṁ;",
  "anya-e1009n12:64": "Lohitaṅga-mayañceva,",
  "anya-e1009n12:65": "Sabbaṁ paṭilabhāmahaṁ.",
  "anya-e1009n12:66": "Ṭha.",
  "anya-e1009n12:67": " Koseyya-kambaliyāni ,",
  "anya-e1009n12:68": "Khoma-kappāsikāni ca;",
  "anya-e1009n12:69": "Mahagghāni ca vatthāni,",
  "anya-e1009n12:70": "Sabbaṁ paṭilabhāmahaṁ.",
  "anya-e1009n12:71": "Ḍa.",
  "anya-e1009n12:72": " Annaṁ pānaṁ khādanīyaṁ,",
  "anya-e1009n12:73": "Vatthaṁ senāsanāni ca;",
  "anya-e1009n12:74": "Sabbametaṁ paṭilabhe,",
  "anya-e1009n12:75": "Uposathassidaṁ phalaṁ.",
  "anya-e1009n12:76": "Ḍha.",
  "anya-e1009n12:77": " Vara-gandhañca mālañca,",
  "anya-e1009n12:78": "Cuṇṇakañcha vilepanaṁ;",
  "anya-e1009n12:79": "Sabbametaṁ paṭilabhe,",
  "anya-e1009n12:80": "Uposathassidaṁ phalaṁ.",
  "anya-e1009n12:81": "Ṇa.",
  "anya-e1009n12:82": " Kūṭāgārañca pāsādaṁ,",
  "anya-e1009n12:83": "Maṇḍapaṁ hammiyaṁ guhaṁ;",
  "anya-e1009n12:84": "Sabbametaṁ paṭilabhe,",
  "anya-e1009n12:85": "Uposathassidaṁ phalaṁ.",
  "anya-e1009n12:86": "Ta.",
  "anya-e1009n12:87": " Jātiyā satta-vassāhaṁ,",
  "anya-e1009n12:88": "Pabbajiṁ ana-gāriyaṁ;",
  "anya-e1009n12:89": "Aḍḍha-māse a-sappatte,",
  "anya-e1009n12:90": "Ara hattaṁ apāpuṇiṁ.",
  "anya-e1009n12:91": "Tha.",
  "anya-e1009n12:92": " Kilesā jhāpitā mayhaṁ,",
  "anya-e1009n12:93": "Bhavā sabbe samūhatā;",
  "anya-e1009n12:94": "Sabbāsava-parikkhīṇā,",
  "anya-e1009n12:95": "Natthi dāni puna-bbhavo.",
  "anya-e1009n12:96": "Da.",
  "anya-e1009n12:97": " Eka-navutito kappe,",
  "anya-e1009n12:98": "Yaṁ kammamakariṁ tadā;",
  "anya-e1009n12:99": "Duggatiṁ nābhijānāmi,",
  "anya-e1009n12:100": "Uposathassidaṁ phalaṁ.",
  "anya-e1009n12:101": "Dha.",
  "anya-e1009n12:102": " Svāgataṁ vata me āsi,",
  "anya-e1009n12:103": "Mama buddhassa santike;",
  "anya-e1009n12:104": "Tisso vijjā anu-pattā,",
  "anya-e1009n12:105": "Kataṁ buddhassa sāsanaṁ.",
  "anya-e1009n12:106": "Na.",
  "anya-e1009n12:107": " Paṭisambhidā catasso,",
  "anya-e1009n12:108": "Vimokāpi ca aṭṭhime;",
  "anya-e1009n12:109": "Chaḷābhiññā sacchikatā,",
  "anya-e1009n12:110": "Kataṁ buddhassa sāsanaṁ.",
  "anya-e1009n12:111": "293.",
  "anya-e1009n12:112": " Ñātīnañca piyo hoti,",
  "anya-e1009n12:113": "Mittesu ca virocati;",
  "anya-e1009n12:114": "Kāyassa bhedā su-gatiṁ,",
  "anya-e1009n12:115": "Upapajjati sīlavā.",
  "anya-e1009n12:116": "294.",
  "anya-e1009n12:117": " Nibbānaṁ patthayantena samādinnaṁ,",
  "anya-e1009n12:118": "Pañca-sīlampi adhi-sīlaṁ;",
  "anya-e1009n12:119": "Dasa-sīlampi adhi-sīlameva.",
  "anya-e1009n12:120": "Ka.",
  "anya-e1009n12:121": " Nagare candavatiyā,",
  "anya-e1009n12:122": "Bhaṭako āsahaṁ tadā;",
  "anya-e1009n12:123": "Para-kammāyane yutto,",
  "anya-e1009n12:124": "Pabbajjaṁ na labhāmahaṁ.",
  "anya-e1009n12:125": "Kha.",
  "anya-e1009n12:126": " Mahandhakāra-pihitā ,",
  "anya-e1009n12:127": "Tividhaggīhi ḍayhare;",
  "anya-e1009n12:128": "Kena nukho upāyena,",
  "anya-e1009n12:129": "Vi-saṁyutto bhave ahaṁ.",
  "anya-e1009n12:130": "Ga.",
  "anya-e1009n12:131": " Deyyadhammo ca me natthi,",
  "anya-e1009n12:132": "Bhaṭako dukkhito ahaṁ;",
  "anya-e1009n12:133": "Yaṁ nūnāhaṁ pañca-sīlaṁ,",
  "anya-e1009n12:134": "Rakkheyyaṁ paripūrayaṁ.",
  "anya-e1009n12:135": "Gha.",
  "anya-e1009n12:136": " Anomadassissa munino,",
  "anya-e1009n12:137": "Nisabho nāma sāvako;",
  "anya-e1009n12:138": "Tamahaṁ upasaṅkamma,",
  "anya-e1009n12:139": "Pañca-sikkhāpadaggahiṁ.",
  "anya-e1009n12:140": "Ṅa.",
  "anya-e1009n12:141": " Vassa-sata-sahassāni ,",
  "anya-e1009n12:142": "Āyu vijjati tāvade;",
  "anya-e1009n12:143": "Tāvatā pañca-sīlāni,",
  "anya-e1009n12:144": "Paripuṇṇāni gopayiṁ.",
  "anya-e1009n12:145": "Ca.",
  "anya-e1009n12:146": " Maccu-kālamhi sampatte,",
  "anya-e1009n12:147": "Devā assāsayanti maṁ;",
  "anya-e1009n12:148": "Ratho sahassa-yutto te,",
  "anya-e1009n12:149": "Mārisassa upaṭṭhito.",
  "anya-e1009n12:150": "Cha.",
  "anya-e1009n12:151": " Vattante carime citte,",
  "anya-e1009n12:152": "Mama sīlaṁ anussariṁ;",
  "anya-e1009n12:153": "Tena kammena su-katena,",
  "anya-e1009n12:154": "Tāvatiṁsaṁ agacchahaṁ.",
  "anya-e1009n12:155": "Ja.",
  "anya-e1009n12:156": " Tiṁsakhattuñca devindo,",
  "anya-e1009n12:157": "Deva-rajjamakārayiṁ;",
  "anya-e1009n12:158": "Dibba-sukhaṁ anubhaviṁ,",
  "anya-e1009n12:159": "Accharāhi purakkhatto.",
  "anya-e1009n12:160": "Jha.",
  "anya-e1009n12:161": " Pañca-sattatikhatttu-ñca ,",
  "anya-e1009n12:162": "Cakkavattī ahosahaṁ;",
  "anya-e1009n12:163": "Padesa-rajjaṁ vipulaṁ,",
  "anya-e1009n12:164": "Gaṇanāto a-saṅkhayaṁ.",
  "anya-e1009n12:165": "Ña.",
  "anya-e1009n12:166": " Deva-lokā cavitvāna,",
  "anya-e1009n12:167": "Sukka-mūlena codito;",
  "anya-e1009n12:168": "Pure vesāliyaṁ jāto,",
  "anya-e1009n12:169": "Mahā-kule su-aḍḍhake.",
  "anya-e1009n12:170": "Ṭa.",
  "anya-e1009n12:171": " Vassūpanāyike kāle,",
  "anya-e1009n12:172": "Dibbante jina-sāsane;",
  "anya-e1009n12:173": "Mātā ca me pitā ceva,",
  "anya-e1009n12:174": "Pañca-sikkhāpadaggahuṁ.",
  "anya-e1009n12:175": "Ṭha.",
  "anya-e1009n12:176": " Saha sutvānahaṁ sīlaṁ,",
  "anya-e1009n12:177": "Mama sīlaṁ anussariṁ;",
  "anya-e1009n12:178": "Ekāsane nisīditvā,",
  "anya-e1009n12:179": "Arahattamapāpuṇiṁ.",
  "anya-e1009n12:180": "Ḍa.",
  "anya-e1009n12:181": " Jātiyā pañca-vassena,",
  "anya-e1009n12:182": "Arahattamapāpuṇiṁ;",
  "anya-e1009n12:183": "Upasampādayi buddho,",
  "anya-e1009n12:184": "Guṇamaññāya cakkhumā.",
  "anya-e1009n12:185": "Ḍha.",
  "anya-e1009n12:186": " Paripuṇṇāni gopetvā,",
  "anya-e1009n12:187": "Pañca-sikkhāpadānahaṁ;",
  "anya-e1009n12:188": "A-parimeyyito kappe,",
  "anya-e1009n12:189": "Vinipātaṁ na gacchahaṁ.",
  "anya-e1009n12:190": "Ṇa.",
  "anya-e1009n12:191": " Svāhaṁ yasamanubhaviṁ,",
  "anya-e1009n12:192": "Tesaṁ sīlāna vāhasā;",
  "anya-e1009n12:193": "Kappa-koṭipi kittento,",
  "anya-e1009n12:194": "Kittaye eka-desakaṁ.",
  "anya-e1009n12:195": "Ta.",
  "anya-e1009n12:196": " Pañca-sīlāni gopetvā,",
  "anya-e1009n12:197": "Tayo hetū labhāmahaṁ;",
  "anya-e1009n12:198": "Dīghāyuko mahā-bhogo,",
  "anya-e1009n12:199": "Tikkha-pañño bhavāmahaṁ.",
  "anya-e1009n12:200": "Tha.",
  "anya-e1009n12:201": " Saṁkittento ca sabbesaṁ,",
  "anya-e1009n12:202": "Adhi-mattañca porisaṁ;",
  "anya-e1009n12:203": "Bhavābhave saṁsaritvā,",
  "anya-e1009n12:204": "Ete ṭhāne labhamahaṁ.",
  "anya-e1009n12:205": "Da.",
  "anya-e1009n12:206": " A-parimeyya-sīlesu ,",
  "anya-e1009n12:207": "Vattantā jina-sāvakā;",
  "anya-e1009n12:208": "Bhavesu yadi rajjeyyuṁ,",
  "anya-e1009n12:209": "Vipāko kīdiso bhave.",
  "anya-e1009n12:210": "Dha.",
  "anya-e1009n12:211": " Su-ciṇṇaṁ me pañca-sīlaṁ,",
  "anya-e1009n12:212": "Bhaṭakena tapassinā;",
  "anya-e1009n12:213": "Tena sīlenahaṁ ajja,",
  "anya-e1009n12:214": "Mocayiṁ sabba-bandhanā.",
  "anya-e1009n12:215": "Na.",
  "anya-e1009n12:216": " A-parimeyyito kappe,",
  "anya-e1009n12:217": "Pañca-sīlāni gopayiṁ;",
  "anya-e1009n12:218": "Duggatiṁ nābhijānāmi,",
  "anya-e1009n12:219": "Pañca-sīlānidaṁ phalaṁ.",
  "anya-e1009n12:220": "Pa.",
  "anya-e1009n12:221": " Paṭisambhidā catasso,",
  "anya-e1009n12:222": "Vimokkhāpi ca aṭṭhime;",
  "anya-e1009n12:223": "Chaḷābhiññā sacchikatā,",
  "anya-e1009n12:224": "Kataṁ buddhassa sāsanaṁ.",
  "anya-e1009n12:225": "Ka.",
  "anya-e1009n12:226": " Taṁ namassanti te vijjā,",
  "anya-e1009n12:227": "Sabbe bhūmā ca khattiyā;",
  "anya-e1009n12:228": "Cattāro ca mahā-rājā,",
  "anya-e1009n12:229": "Tidasā ca yasassino;",
  "anya-e1009n12:230": "Atha ko nāma so yakkho,",
  "anya-e1009n12:231": "Yaṁ tvaṁ sakka namassasi.",
  "anya-e1009n12:232": "Kha.",
  "anya-e1009n12:233": " Maṁ namassanti te-vijjā,",
  "anya-e1009n12:234": "Sabbe bhūmā ca khattiyā;",
  "anya-e1009n12:235": "Cattāro ca mahā-rājā,",
  "anya-e1009n12:236": "Tidasā ca yasassino.",
  "anya-e1009n12:237": "Ga.",
  "anya-e1009n12:238": " Ahañca sīla-sampanne,",
  "anya-e1009n12:239": "Ciraratta-samāhite;",
  "anya-e1009n12:240": "Sammā pabbajite vande,",
  "anya-e1009n12:241": "Brahma-cariya parāyane.",
  "anya-e1009n12:242": "Gha.",
  "anya-e1009n12:243": " Ye gahaṭṭhā puñña-karā,",
  "anya-e1009n12:244": "Sīlavanto upāsakā;",
  "anya-e1009n12:245": "Dhammena dāraṁ posenti,",
  "anya-e1009n12:246": "Te namassāmi mātali.",
  "anya-e1009n12:247": "Ṅa.",
  "anya-e1009n12:248": " Seṭṭhā hi kira lokasmiṁ,",
  "anya-e1009n12:249": "Ye tvaṁ sakka namassasi;",
  "anya-e1009n12:250": "Ahampi te namassāmi,",
  "anya-e1009n12:251": "Ye namassasi vāsava.",
  "anya-e1009n12:252": "1.",
  "anya-e1009n12:253": " Pālitattheranāgena ,",
  "anya-e1009n12:254": "Visuddhārāmavāsinā;",
  "anya-e1009n12:255": "Suticchitānamatthāya,",
  "anya-e1009n12:256": "Katā naradakkhadīpanī.",
  "anya-e1009n12:257": "2.",
  "anya-e1009n12:258": " Pubbācariya-sīhānaṁ ,",
  "anya-e1009n12:259": "Ālambitvāna nissayaṁ;",
  "anya-e1009n12:260": "Pālito nāma yo thero,",
  "anya-e1009n12:261": "Imaṁ gantha su-lekhanī;",
  "anya-e1009n12:262": "Sundarameva passituṁ,",
  "anya-e1009n12:263": "Yuñjeyyāthīdha sādhave.",
  "anya-e1009n12:264": "‘‘Chappadikā’’.",
  "anya-e1009n12:265": "3.",
  "anya-e1009n12:266": " Imaṁ ganthaṁ vācuggato,",
  "anya-e1009n12:267": "Sace bhavasi māṇava;",
  "anya-e1009n12:268": "Puṇnamāyaṁ yathā cando,",
  "anya-e1009n12:269": "Ati-suddho virocati;",
  "anya-e1009n12:270": "Tatheva tvaṁ puṇṇa-mano,",
  "anya-e1009n12:271": "Viroca siriyā dhuvaṁ.",
  "anya-e1009n12:272": "4.",
  "anya-e1009n12:273": " Su-niṭṭhito ayaṁ gantho,",
  "anya-e1009n12:274": "Sakkarāje dajhamphiye;",
  "anya-e1009n12:275": "Poṭṭhapādamhi sūramhi,",
  "anya-e1009n12:276": "Kālapakkhe catuddasiṁ.",
  "anya-e1009n12:277": "5.",
  "anya-e1009n12:278": " Sañcitetaṁ mayā puññaṁ,",
  "anya-e1009n12:279": "Taṁ-kammena varena ca;",
  "anya-e1009n12:280": "Ciraṁ tiṭṭhatu saddhammo,",
  "anya-e1009n12:281": "A-verā hontu pāṇino.",
  "anya-e1009n12:282": "6.",
  "anya-e1009n12:283": " Imaṁ ganthaṁ passitvāna,",
  "anya-e1009n12:284": "Hontu sabbepi jantuno;",
  "anya-e1009n12:285": "Sukhitā dhammikā ñāṇī,",
  "anya-e1009n12:286": "Dhammaṁ pāletu patthivo.",
  "anya-e1009n12:287": "7.",
  "anya-e1009n12:288": " Nibbānaṁ patthayantena,",
  "anya-e1009n12:289": "Sīlaṁ rakkhantu sajjanā;",
  "anya-e1009n12:290": "Ñatvā dhammaṁ sukhāvahaṁ,",
  "anya-e1009n12:291": "Pāpuṇantu anāsavaṁ.",
  "anya-e1009n12:292": "8.",
  "anya-e1009n12:293": " Aṭṭha-kaṇḍa-maṇḍitāya ,",
  "anya-e1009n12:294": "Dakkhaya attha-dīpako;",
  "anya-e1009n12:295": "Nara-sāro ayaṁ gantho,",
  "anya-e1009n12:296": "Cira-kālaṁ patiṭṭhatu.",
  "anya-e1009n12:297": "9.",
  "anya-e1009n12:298": " Yāvatā canda-sūriyā,",
  "anya-e1009n12:299": "Nāgaccheyyuṁ mahī-tale;",
  "anya-e1009n12:300": "Pamoditā imaṁ ganthaṁ,",
  "anya-e1009n12:301": "Dissantu naya-kovidā.",
  "anya-e1009n12:302": "10.",
  "anya-e1009n12:303": " Sammā chandenimaṁ gantha,",
  "anya-e1009n12:304": "Vācentā pariyāpuṇā;",
  "anya-e1009n12:305": "Pasannenānāyāsena,",
  "anya-e1009n12:306": "Patvā sukhena kovidaṁ.",
  "anya-e1009n12:307": "11.",
  "anya-e1009n12:308": " Candādiccāva ākāse,",
  "anya-e1009n12:309": "Bahussutehi sampadā;",
  "anya-e1009n12:310": "Visesa-puggalā hutvā,",
  "anya-e1009n12:311": "Pappontu amataṁ padaṁ.",
  "anya-e1009n12:312": "12.",
  "anya-e1009n12:313": " Ukkaṭṭha-dhamma-dānena ,",
  "anya-e1009n12:314": "Pāpuṇeyyamanuttaraṁ;",
  "anya-e1009n12:315": "Liṅga-sampatti-medhāvī,",
  "anya-e1009n12:316": "Takkī-paññā su-pesalī."
}