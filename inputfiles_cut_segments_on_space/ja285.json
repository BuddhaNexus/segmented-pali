{
  "ja285:0.1": "Jātaka ",
  "ja285:0.2": "Tikanipāta ",
  "ja285:0.3": "Abbhantaravagga ",
  "ja285:0.4": "5. Maṇisūkarajātaka ",
  "ja285:1.1": "“Dariyā satta vassāni, ",
  "ja285:1.2": "tiṁsamattā vasāmase; ",
  "ja285:1.3": "Haññāma maṇino ābhaṁ, ",
  "ja285:1.4": "iti no mantanaṁ ahu. ",
  "ja285:2.1": "Yāvatā maṇiṁ ghaṁsāma, ",
  "ja285:2.2": "bhiyyo vodāyate maṇi; ",
  "ja285:2.3": "Idañca dāni pucchāma, ",
  "ja285:2.4": "kiṁ kiccaṁ idha maññasi”. ",
  "ja285:3.1": "“Ayaṁ maṇi veḷūriyo, ",
  "ja285:3.2": "akāco vimalo subho; ",
  "ja285:3.3": "Nāssa sakkā siriṁ hantuṁ, ",
  "ja285:3.4": "apakkamatha sūkarā”ti. ",
  "ja285:4.1": "Maṇisūkarajātakaṁ pañcamaṁ. "
}