{
  "pv48:0.1": "Petavatthu ",
  "pv48:0.2": "Mahāvagga ",
  "pv48:0.3": "13. Akkharukkhapetavatthu ",
  "pv48:1.1": "“Yaṁ dadāti na taṁ hoti, ",
  "pv48:1.2": "Detheva dānaṁ datvā ubhayaṁ tarati; ",
  "pv48:1.3": "Ubhayaṁ tena dānena gacchati, ",
  "pv48:1.4": "Jāgaratha māpamajjathā”ti. ",
  "pv48:2.1": "Akkharukkhapetavatthu terasamaṁ. "
}