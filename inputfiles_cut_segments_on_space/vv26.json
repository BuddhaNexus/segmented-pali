{
  "vv26:0.1": "Vimānavatthu ",
  "vv26:0.2": "Itthivimāna ",
  "vv26:0.3": "Cittalatāvagga ",
  "vv26:0.4": "9. Suniddāvimānavatthu ",
  "vv26:1.1": "“Abhikkantena vaṇṇena, ",
  "vv26:1.2": "…pe… ",
  "vv26:1.3": "osadhī viya tārakā. ",
  "vv26:2.1": "Kena tetādiso vaṇṇo, ",
  "vv26:2.2": "…pe… ",
  "vv26:2.3": "vaṇṇo ca te sabbadisā pabhāsatī”ti. ",
  "vv26:3.1": "Sā devatā attamanā, ",
  "vv26:3.2": "…pe… ",
  "vv26:3.3": "yassa kammassidaṁ phalaṁ. ",
  "vv26:4.1": "“Suniddāti maṁ aññaṁsu, ",
  "vv26:4.2": "rājagahasmiṁ upāsikā; ",
  "vv26:4.3": "Saddhā sīlena sampannā, ",
  "vv26:4.4": "saṁvibhāgaratā sadā. ",
  "vv26:5.1": "(…) ",
  "vv26:6.1": "Pañcasikkhāpade ratā, ",
  "vv26:6.2": "ariyasaccāna kovidā; ",
  "vv26:6.3": "Upāsikā cakkhumato, ",
  "vv26:6.4": "gotamassa yasassino. ",
  "vv26:7.1": "Tena metādiso vaṇṇo, ",
  "vv26:7.2": "…pe… ",
  "vv26:7.3": "vaṇṇo ca me sabbadisā pabhāsatī”ti. ",
  "vv26:8.1": "Suniddāvimānaṁ navamaṁ. "
}