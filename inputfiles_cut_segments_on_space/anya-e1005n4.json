{
  "anya-e1005n4:0": "5. Itthikaṇḍo",
  "anya-e1005n4:1": "94.",
  "anya-e1005n4:2": "Kokilānaṁ saddaṁ rūpaṁ,",
  "anya-e1005n4:3": "Nārīrūpaṁ patibbatā;",
  "anya-e1005n4:4": "Vijjā rūpaṁ a-rūpānaṁ,",
  "anya-e1005n4:5": "Khamā rūpaṁ tapassinaṁ.",
  "anya-e1005n4:6": "95.",
  "anya-e1005n4:7": "Itthīnañca dhanaṁ rūpaṁ,",
  "anya-e1005n4:8": "Purisānaṁ vijjā dhanaṁ;",
  "anya-e1005n4:9": "Bhikkhūnañca dhanaṁ sīlaṁ,",
  "anya-e1005n4:10": "Rājānañca dhanaṁ balaṁ.",
  "anya-e1005n4:11": "96.",
  "anya-e1005n4:12": "Tapassino kisā sobhā,",
  "anya-e1005n4:13": "Thūlā sobhā catuppadā;",
  "anya-e1005n4:14": "Purisā vijjavā sobhā,",
  "anya-e1005n4:15": "Itthī sobhāsa sāmikā.",
  "anya-e1005n4:16": "97.",
  "anya-e1005n4:17": "Pañca ratyā sugandhabbā,",
  "anya-e1005n4:18": "Satta ratyā dhanuggahā;",
  "anya-e1005n4:19": "Eka māsā subhariyā,",
  "anya-e1005n4:20": "Aḍḍha māsā sissā malā.",
  "anya-e1005n4:21": "98.",
  "anya-e1005n4:22": "Hiṁ ramati paṅa,",
  "anya-e1005n4:23": "Haṅa ramati poka.",
  "anya-e1005n4:24": "Thī ramati pu,",
  "anya-e1005n4:25": "Khu ramati dhaṁ.",
  "anya-e1005n4:26": "99.",
  "anya-e1005n4:27": "Jiṇṇamannaṁ pasaṁseyya,",
  "anya-e1005n4:28": "Dārañca gatayobbanaṁ;",
  "anya-e1005n4:29": "Raṇā punāgatā sūraṁ,",
  "anya-e1005n4:30": "Sassañca gehamāgataṁ.",
  "anya-e1005n4:31": "100.",
  "anya-e1005n4:32": "Dvatti-patikā nārī ca,",
  "anya-e1005n4:33": "Bhikkhu dvatti-vihāriko;",
  "anya-e1005n4:34": "Dvatti-pāsa-mutto pakkhī,",
  "anya-e1005n4:35": "Kata-māyā bahūtharaṁ.",
  "anya-e1005n4:36": "101.",
  "anya-e1005n4:37": "Dujjanaṁ pahārādame,",
  "anya-e1005n4:38": "Mittaṁ dame a-bhāṇikā;",
  "anya-e1005n4:39": "Itthiñca byasanā dame,",
  "anya-e1005n4:40": "Rāginaṁ appa bhojanā.",
  "anya-e1005n4:41": "102.",
  "anya-e1005n4:42": "Na ratti vinā candimā,",
  "anya-e1005n4:43": "Vīciṁ vinā ca aṇṇavo;",
  "anya-e1005n4:44": "Haṁsaṁ vinā pokkharaṇī,",
  "anya-e1005n4:45": "Patiṁ kaññāca sobhate.",
  "anya-e1005n4:46": "103.",
  "anya-e1005n4:47": "Patinā janito bhogo,",
  "anya-e1005n4:48": "Itthiyāva saṁgoppito;",
  "anya-e1005n4:49": "Purisova hi padhāno,",
  "anya-e1005n4:50": "Itthī suttaṁva sūciyā.",
  "anya-e1005n4:51": "104.",
  "anya-e1005n4:52": "Sabbānadī vaṅkanadī,",
  "anya-e1005n4:53": "Sabbe kaṭṭhamayā vanā;",
  "anya-e1005n4:54": "Sabbitthiyo kare pāpaṁ,",
  "anya-e1005n4:55": "Labhamāne nivātake.",
  "anya-e1005n4:56": "105.",
  "anya-e1005n4:57": "Vivādasīliṁ usūyabhāṇiniṁ,",
  "anya-e1005n4:58": "Sampassataṇhiṁ bahupākabhuttiniṁ;",
  "anya-e1005n4:59": "Aggantabhuttiṁ paragehavāsiniṁ,",
  "anya-e1005n4:60": "Nāriṁ caje puttasatampi pūmā.",
  "anya-e1005n4:61": "106.",
  "anya-e1005n4:62": "Bhuttesu maṇḍesu janīva kantinī,",
  "anya-e1005n4:63": "Guyheca ṭhāne bhaginīva hiriṇī;",
  "anya-e1005n4:64": "Kammesu pattesu karoti dāsīva,",
  "anya-e1005n4:65": "Bhayesu mantī sayanesu rāmaye;",
  "anya-e1005n4:66": "Rūpīsu sikkhī kupanesu khantinī,",
  "anya-e1005n4:67": "Sā nārī seṭṭhāti vadanti paṇḍitā;",
  "anya-e1005n4:68": "Kāyassa bhedāca divebhaveyya sā.",
  "anya-e1005n4:69": "107.",
  "anya-e1005n4:70": "Sāmā migakkhī tanumajjhagattā,",
  "anya-e1005n4:71": "Sūrū sukesī samadantapantī;",
  "anya-e1005n4:72": "Gambhīranābhī yuvatī susīlī,",
  "anya-e1005n4:73": "Hīne kule jātāpi vivāhyā.",
  "anya-e1005n4:74": "108.",
  "anya-e1005n4:75": "Saradaṁratu-kālānaṁ,",
  "anya-e1005n4:76": "Bhariyānaṁ rūpavatī;",
  "anya-e1005n4:77": "Jeṭṭho padhānaṁ puttānaṁ,",
  "anya-e1005n4:78": "Disānaṁ uttarādisā.",
  "anya-e1005n4:79": "109.",
  "anya-e1005n4:80": "Yā icche puriso hotuṁ,",
  "anya-e1005n4:81": "Jāti jāti punappunaṁ;",
  "anya-e1005n4:82": "Sāmikaṁ apacāyeyya,",
  "anya-e1005n4:83": "Indaṁva pāricārikā.",
  "anya-e1005n4:84": "110.",
  "anya-e1005n4:85": "Yo icche puriso hotuṁ,",
  "anya-e1005n4:86": "Jāti jāti punappunaṁ;",
  "anya-e1005n4:87": "Paradānaṁ vivajjeyya,",
  "anya-e1005n4:88": "Dhotapādova kaddamaṁ.",
  "anya-e1005n4:89": "111.",
  "anya-e1005n4:90": "Atikkanta vayo poso,",
  "anya-e1005n4:91": "Āneti timbaruttaniṁ;",
  "anya-e1005n4:92": "Tassā issā asaddhāti,",
  "anya-e1005n4:93": "Taṁ parābhavato mukhaṁ.",
  "anya-e1005n4:94": "Itthikaṇḍo niṭṭhito."
}