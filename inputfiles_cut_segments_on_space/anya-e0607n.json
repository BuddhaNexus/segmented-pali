{
  "anya-e0607n:0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e0607n:1": "Pajjamadhu",
  "anya-e0607n:2": "Ānanda rañña ratanādi mahā yatinda,",
  "anya-e0607n:3": "Niccappabuddha padumappiya sevinaṅgī;",
  "anya-e0607n:4": "Buddhappiyena ghana buddha guṇappiyena,",
  "anya-e0607n:5": "Therālinā racita pajjamadhuṃ pibantu.",
  "anya-e0607n:6": "1.",
  "anya-e0607n:7": "Uṇṇāpapuṇṇasasimaṇdalato galitvā,",
  "anya-e0607n:8": "Pādambujaṅguli dalaṭṭha sudhā lavānaṃ;",
  "anya-e0607n:9": "Pantīva satthu nakhapanti pajāvisesaṃ,",
  "anya-e0607n:10": "Pīṇetu suddha sukhitammaṇa tunḍapītā.",
  "anya-e0607n:11": "2.",
  "anya-e0607n:12": "Khittāya māraripunā parivatya satthu,",
  "anya-e0607n:13": "Pādassayā jita disāya sitattalāya;",
  "anya-e0607n:14": "Yā jeti kañcana sarāvaliyā siriṃ sā,",
  "anya-e0607n:15": "Detanginaṃ raṇajayaṅgulipantikantā.",
  "anya-e0607n:16": "3.",
  "anya-e0607n:17": "Sovaṇṇa vaṇṇa sukhumacchavi somma kumma,",
  "anya-e0607n:18": "Piṭṭhīva piṭṭhi kamatunnati bhāti yesaṃ;",
  "anya-e0607n:19": "Tesuppatiṭṭhitasukomaladīghapaṇhi,",
  "anya-e0607n:20": "Pādā jinassa padadantu padaṃ janassa.",
  "anya-e0607n:21": "4.",
  "anya-e0607n:22": "Acchera paṅkajasiraṃ siriyā sakāya,",
  "anya-e0607n:23": "Ye maddino viya caranti saroja sīse;",
  "anya-e0607n:24": "Sañcumbitā viya ca tāni parāga rāgā,",
  "anya-e0607n:25": "Te nīrajā munipadā padadantu lakkhiṃ.",
  "anya-e0607n:26": "5.",
  "anya-e0607n:27": "Agāmi kāla jana maṅgala bhattu bhāvaṃ,",
  "anya-e0607n:28": "Vyākattumatra kusaleniva nimmitāni;",
  "anya-e0607n:29": "Yātrāsumaṭṭhasatamaṅgalakkhaṇāni,",
  "anya-e0607n:30": "Sādhetu naṃ padayugaṃ jayamaṅgalāni.",
  "anya-e0607n:31": "6.",
  "anya-e0607n:32": "Sassevijantuvarasantipurappavese,",
  "anya-e0607n:33": "Niccaṃ susajja ṭhapitāniva maṅgalāya;",
  "anya-e0607n:34": "Ye te dadhanti kalamaṅgalalakkhaṇāni,",
  "anya-e0607n:35": "Vattantu te jinapadā jayamaṅgalāya.",
  "anya-e0607n:36": "7.",
  "anya-e0607n:37": "Sabbebhibhūya sapadesu nipātanassa,",
  "anya-e0607n:38": "Saññāṇakaṃ viya yadassitasabbaloko;",
  "anya-e0607n:39": "Pādātyadhokatatilokasirovarā pi,",
  "anya-e0607n:40": "Lokaṃ puṇantu jayamaṅgalakāraṇāni.",
  "anya-e0607n:41": "8.",
  "anya-e0607n:42": "Lokattayekasaraṇattavibhāvanāya,",
  "anya-e0607n:43": "Sajjo va tiṭṭhati yahiṃ suvibhattaloko;",
  "anya-e0607n:44": "Taṃ sabbalokapaṭibimbitadappaṇābhaṃ,",
  "anya-e0607n:45": "Pādadvayaṃ janasusajjanahetu hotu.",
  "anya-e0607n:46": "9.",
  "anya-e0607n:47": "Lokuttarāya siriyādhigamāya suṭṭhu,",
  "anya-e0607n:48": "Rajanti yattha diguṇāniva pātu bhūtā;",
  "anya-e0607n:49": "Cakkāsanābhisahanemisahassarāni,",
  "anya-e0607n:50": "Tyaṅghī disantu sakalissariyaṃ janassa.",
  "anya-e0607n:51": "10.",
  "anya-e0607n:52": "Yatrullasanti duvidhāniva pātū bhūtā,",
  "anya-e0607n:53": "Dhammassasabbabhuvanassa ca issaratte;",
  "anya-e0607n:54": "Cakkāni cakkasadisāni sudassanassa,",
  "anya-e0607n:55": "Tānajja jantu saraṇā caraṇāni hontu.",
  "anya-e0607n:56": "11.",
  "anya-e0607n:57": "Sattesu vacchatu sirī sirivacchakena,",
  "anya-e0607n:58": "Sovatthi sotthimanutiṭṭhatu puggalesu;",
  "anya-e0607n:59": "Nandiṃ janānamanuvattatu nandivattī,",
  "anya-e0607n:60": "Sīsānalaṃkurutu pādavataṃsako pi.",
  "anya-e0607n:61": "12.",
  "anya-e0607n:62": "Bhaddāya pīṭhamupagacchatu bhaddapīṭhaṃ,",
  "anya-e0607n:63": "Vuddhiṃ janānamanuvattatu vaddhamānaṃ;",
  "anya-e0607n:64": "Puṇṇattamaṅgimanukubbatu puṇṇakumbho,",
  "anya-e0607n:65": "Pāti ca pātu satataṃ janataṃ apāyā.",
  "anya-e0607n:66": "13.",
  "anya-e0607n:67": "Setātapattamapanetamaghātape taṃ,",
  "anya-e0607n:68": "Khaggo vichindatu sadā duritārivagge;",
  "anya-e0607n:69": "Saṃklesadāhamapanetu satālavaṇṭa,",
  "anya-e0607n:70": "Saṃvījanī kumatimakkhikamorahattho.",
  "anya-e0607n:71": "14.",
  "anya-e0607n:72": "Ākaḍḍhano janavilocanamattaninnaṃ,",
  "anya-e0607n:73": "Vāretu sabbagativāranamaṅkuso so;",
  "anya-e0607n:74": "Pādambujassirivilāsaniketanaṃ va,",
  "anya-e0607n:75": "Pāsādalakhaṇamupetu manopasādaṃ.",
  "anya-e0607n:76": "15.",
  "anya-e0607n:77": "Pāṇīnamattabhajataṃ varapuṇṇapattaṃ,",
  "anya-e0607n:78": "Sammā dadātu padanissitapuṇṇapatto;",
  "anya-e0607n:79": "Pādesu jantu manabandhanadāmabhūtaṃ,",
  "anya-e0607n:80": "Dāmaṃ dametu vimalaṃ janatammanāni.",
  "anya-e0607n:81": "16.",
  "anya-e0607n:82": "Uṇhīsakuppalamaṇīpadumehi pādā,",
  "anya-e0607n:83": "Sassevijantukaraṇāni vibhūsayantu;",
  "anya-e0607n:84": "Sannettanāvupagatānamanagghakāni,",
  "anya-e0607n:85": "Bojjhaṅgasattaratanāni dade samuddo.",
  "anya-e0607n:86": "17.",
  "anya-e0607n:87": "Uttuṅga niccalaguṅā jitatāya niccaṃ,",
  "anya-e0607n:88": "Sevīva pādasiri nicca samubbahaṃ va;",
  "anya-e0607n:89": "Atrāpi sakkabhavanubbahaṇe niyutto,",
  "anya-e0607n:90": "Pādaṭṭhameru bhavataṃ bhavataṃ vibhūtyā.",
  "anya-e0607n:91": "18.",
  "anya-e0607n:92": "So cakkavāḷasikharī pyavataṃ samantā,",
  "anya-e0607n:93": "Sabbūpasaggavisarājanataṃ samaggaṃ;",
  "anya-e0607n:94": "Dīpā puthūpi caturo dvisahassa khuddā,",
  "anya-e0607n:95": "Dhārentvapāyapatamānamadatva jantuṃ.",
  "anya-e0607n:96": "19.",
  "anya-e0607n:97": "Sūro pabodhayatu jantu saroruhāni,",
  "anya-e0607n:98": "Cando pasāda kumudāni manodahesu;",
  "anya-e0607n:99": "Nakkhattajātamakhilaṃ subhatāya hotu,",
  "anya-e0607n:100": "Cakkaṃ dhajaṃ ripujayāya jayaddhajāya.",
  "anya-e0607n:101": "20.",
  "anya-e0607n:102": "Jetuṃ sasaṃsada-sudassana-cakkavatti,",
  "anya-e0607n:103": "Cakkānugantalalitaṃ yahimāvaheyya;",
  "anya-e0607n:104": "Cakkāṇuvatti parisāvata-cakkavatti,",
  "anya-e0607n:105": "Naṃvattataṃ padayugaṃ janatā hitāya.",
  "anya-e0607n:106": "21.",
  "anya-e0607n:107": "Pujetumāgata vatā vajirāsanaṭṭha,",
  "anya-e0607n:108": "Mindena chaḍḍita mahāvijayuttarākhyaṃ;",
  "anya-e0607n:109": "Saṃkhaṃ paviṭṭhamiva mārabhayā padādho,",
  "anya-e0607n:110": "Pādaṭṭhasaṃkhamiha vattatu santiyā vo.",
  "anya-e0607n:111": "22.",
  "anya-e0607n:112": "Sovaṇṇamacchayugalaṃ sivabhatta bhoge,",
  "anya-e0607n:113": "Icchā bahūpakaraṇaṃ bhavataṃ janānaṃ;",
  "anya-e0607n:114": "Kumbhīladhiggahitato va padutthacittā,",
  "anya-e0607n:115": "Pādambujākara vigāhi tu nopahontu.",
  "anya-e0607n:116": "23.",
  "anya-e0607n:117": "Sattāpagā janamanoja male jahantu,",
  "anya-e0607n:118": "Saṃklesadāhamapanentu dahā ca satta;",
  "anya-e0607n:119": "Selā ca satta vidadhantu janassa tānaṃ,",
  "anya-e0607n:120": "Lokappasiddhijanane bhavataṃ patākā.",
  "anya-e0607n:121": "24.",
  "anya-e0607n:122": "Pāṭaṅki santi gamane bhavatūpakārā,",
  "anya-e0607n:123": "Dāhettanesu jahataṃ padacāmaraṃ taṃ;",
  "anya-e0607n:124": "Sallokalocanamahussava-ussitaṃ va,",
  "anya-e0607n:125": "Vatteyya toraṇamanuttaramaṅgalāya.",
  "anya-e0607n:126": "25.",
  "anya-e0607n:127": "Yasmiṃ miginda gata bhīti balāva daḍḍha,",
  "anya-e0607n:128": "Dānā natā siravidāraṇa pīḷitāva;",
  "anya-e0607n:129": "Nālāgirī karivaro girimekhalo ca,",
  "anya-e0607n:130": "Taṃ sīhavikkamapadaṃ hanatā ghadantiṃ.",
  "anya-e0607n:131": "26.",
  "anya-e0607n:132": "Pāpāhino hanatu pādasuvaṇṇarājā,",
  "anya-e0607n:133": "Vyagghādhipo kalijane adataṃ asesaṃ;",
  "anya-e0607n:134": "Vālāha-assapati sampatituṃ adatvā,",
  "anya-e0607n:135": "Pāyesu pāpayatu santipurampajāyo.",
  "anya-e0607n:136": "27.",
  "anya-e0607n:137": "Chaddanta danti lalitaṃ galitaṃ rusamhā,",
  "anya-e0607n:138": "Luddetta dubbhini dise acalaṃ dadhāno;",
  "anya-e0607n:139": "Pādaṭṭhahatthipati sampati jantutāse,",
  "anya-e0607n:140": "Tāsetu hāsamaparandisataṃ satānaṃ.",
  "anya-e0607n:141": "28.",
  "anya-e0607n:142": "Sabbaṅgino caraṇuposatha hatthirājā,",
  "anya-e0607n:143": "Pāpetu sabbacatudīpikarajjalakkhiṃ;",
  "anya-e0607n:144": "Kittīva pādaparicārikatā niyuttā,",
  "anya-e0607n:145": "Kelasaselapaṭimā hitamācareyya.",
  "anya-e0607n:146": "29.",
  "anya-e0607n:147": "Sāmissa haṃsasamaye dahapāsabaddha,",
  "anya-e0607n:148": "Māsīna vesagamako viya pādahaṃso;",
  "anya-e0607n:149": "Nigghosa gantijitato viya mūgapakkho,",
  "anya-e0607n:150": "Yāretu sabba janatā bhavagantukattaṃ.",
  "anya-e0607n:151": "30.",
  "anya-e0607n:152": "Ohāya dibbasarasiṃ khilaloka sabba,",
  "anya-e0607n:153": "Rammaṅghivāpimavagāhitavāva pāde;",
  "anya-e0607n:154": "Erāvaṇo karivaro manasābhiruḷhe,",
  "anya-e0607n:155": "Jantuṃ purindadapuraṃ nayataṃ va sīghaṃ.",
  "anya-e0607n:156": "31.",
  "anya-e0607n:157": "Hitvā sakambhavanamaṅghinisevanattha,",
  "anya-e0607n:158": "Māgamma ramma taratāyiha nissito va;",
  "anya-e0607n:159": "Pāletva mūni padavāpitaraṅgabhaṅgi,",
  "anya-e0607n:160": "Mangī karontatanuvāsuki nāgarājā.",
  "anya-e0607n:161": "32.",
  "anya-e0607n:162": "Nāthassa kañcanasikhāvalajātilīla,",
  "anya-e0607n:163": "Māvikaraṃ va padanissitamorarājā;",
  "anya-e0607n:164": "Taṃ dhammadesanaraveniva luddakassa,",
  "anya-e0607n:165": "Lokassa pāpaphaṇino hanataṃ asesaṃ.",
  "anya-e0607n:166": "33.",
  "anya-e0607n:167": "Saṃsārasāgaragate sadhane jane te,",
  "anya-e0607n:168": "Netampade kalacatummukhahemanāvā;",
  "anya-e0607n:169": "Nibbāṇapaṭṭanavaraṃ bharukacchakantaṃ,",
  "anya-e0607n:170": "Suppārapaṇḍita gatā viya āsunāvā.",
  "anya-e0607n:171": "34.",
  "anya-e0607n:172": "Sambodhi ñāṇa paripācayato munissa,",
  "anya-e0607n:173": "Bhatto yathā himavataddi samādhihetu;",
  "anya-e0607n:174": "Evammanena bhajataṃ himavaddipāde,",
  "anya-e0607n:175": "Sambodhiñāṇa paripācanahetu hotu.",
  "anya-e0607n:176": "35.",
  "anya-e0607n:177": "Daḷhaṃ parājitatayā muninā sarena,",
  "anya-e0607n:178": "Suññassaropagata pañjara bandhanova;",
  "anya-e0607n:179": "So pādapañjaragato karavīkapakkhī,",
  "anya-e0607n:180": "Sabbesamappīyāvacañjahatā bhavantaṃ.",
  "anya-e0607n:181": "36.",
  "anya-e0607n:182": "Te cakkavāka makarā api koñca jīvaṃ,",
  "anya-e0607n:183": "Jīvādi pakkhivisarā sarasīva bhuttaṃ;",
  "anya-e0607n:184": "Vessantarena caraṇambuji nibbhajantā,",
  "anya-e0607n:185": "Jantu tahiṃ viya pade suramentu niccaṃ.",
  "anya-e0607n:186": "37.",
  "anya-e0607n:187": "Taṃ candakinnaragatiṃva gatassa bodhi,",
  "anya-e0607n:188": "Sattassa tassa sapajāpatikassa bhāvaṃ;",
  "anya-e0607n:189": "Saṃsūcayanta pada kinnara kinnarī ve,",
  "anya-e0607n:190": "Sāmaggimagga paṭi pattisu pāpayantu.",
  "anya-e0607n:191": "38.",
  "anya-e0607n:192": "Saṃrājadhānimusabho vahatagga bhāraṃ,",
  "anya-e0607n:193": "Pītippayo pajanayeyya savacchadhenu;",
  "anya-e0607n:194": "Sassevino abhiramentu chakāmasaggā,",
  "anya-e0607n:195": "Dhārentu jhāyimiha soḷasa dhātudhāmā.",
  "anya-e0607n:196": "39.",
  "anya-e0607n:197": "Sutvā jinassa karavīka sarammanuññaṃ,",
  "anya-e0607n:198": "Aññoñña bhītirahitā api paccanīkā;",
  "anya-e0607n:199": "Hitvā gatiṃ viya ṭhitā padasattarūpā,",
  "anya-e0607n:200": "Sabbaṃ bhavassita janānagatiṃ hanantu.",
  "anya-e0607n:201": "40.",
  "anya-e0607n:202": "Sovaṇṇa kāhaḷa yugo pamamindirāya,",
  "anya-e0607n:203": "Sannīrapuppha mukulopamamussavāya;",
  "anya-e0607n:204": "Niccaṃ susajja ṭhapitaṃ muni tiṭṭhatante,",
  "anya-e0607n:205": "Jaṅghādvayaṃ janavilocana maṅgalāya.",
  "anya-e0607n:206": "41.",
  "anya-e0607n:207": "Lakhyā vilāsa mukuradvaya sannikāsaṃ,",
  "anya-e0607n:208": "Tāḍaṅka maṇḍana viḍambakamaṃsu saṇḍaṃ;",
  "anya-e0607n:209": "Jānudvayaṃ laḷita sāgara bubbalābhaṃ,",
  "anya-e0607n:210": "Hotaṃ jagattaya nijatta vibhūsitunte.",
  "anya-e0607n:211": "42.",
  "anya-e0607n:212": "Chaddanti dinna varadanta yugopamānā,",
  "anya-e0607n:213": "Taṃ hatthi soṇḍa kama puṇṇa guṇā tavorū;",
  "anya-e0607n:214": "Līla payodhi siri keḷi suvaṇṇarambhā,",
  "anya-e0607n:215": "Khandhāva dentu paripuṇṇa gune janānāṃ.",
  "anya-e0607n:216": "43.",
  "anya-e0607n:217": "Jaṅghakkha kadvya samappita cittapāda,",
  "anya-e0607n:218": "Cakkadvayī manamanojahayo mune te;",
  "anya-e0607n:219": "Sonī ratho sirivaho manasā bhiruḷhaṃ,",
  "anya-e0607n:220": "Lokattayaṃ sivapuraṃ lahu pāpayātu.",
  "anya-e0607n:221": "44.",
  "anya-e0607n:222": "Rammora pākaṭa taṭāka taṭā savanta,",
  "anya-e0607n:223": "Romāvalī jala panālika koṭikaṭṭhā;",
  "anya-e0607n:224": "Nābhī gabhīra sarasī siri keḷitā te,",
  "anya-e0607n:225": "Sassevinaṃ vyasana ghammamalaṃ sametu.",
  "anya-e0607n:226": "45.",
  "anya-e0607n:227": "Kanticchaṭā luḷita rūpa payodhi nābhi,",
  "anya-e0607n:228": "Āvaṭṭa vaṭṭita nimujjita sabbaloko;",
  "anya-e0607n:229": "Sobhagga toya nivahaṃ visaso pivitvā,",
  "anya-e0607n:230": "Lokuttarādi sukha mucchitataṃ payātu.",
  "anya-e0607n:231": "46.",
  "anya-e0607n:232": "Gambhīra cittarahadaṃ paripūrayitvā,",
  "anya-e0607n:233": "Taṃ sandamāna karuṇambu pavāha tulyā;",
  "anya-e0607n:234": "Romālivallihari nābhi subhālavālā,",
  "anya-e0607n:235": "Detaṃ lahuṃ sivaphalaṃ bhajataṃ mune te.",
  "anya-e0607n:236": "47.",
  "anya-e0607n:237": "Cārūra sāriphalako kuṭilagga loma,",
  "anya-e0607n:238": "Pantī vibhatti sahito siri keḷi sajjo;",
  "anya-e0607n:239": "Saggāpavagga sukha jūtaka keḷi hetu,",
  "anya-e0607n:240": "Hotaṃ tiloka sukha jūtaka soṇḍakānaṃ.",
  "anya-e0607n:241": "48.",
  "anya-e0607n:242": "Gambhīra citta rahado dara gāhamāna,",
  "anya-e0607n:243": "Mettādayā kari vadhū kara sanni kāsā;",
  "anya-e0607n:244": "Sabbaṅginaṃ sivaphalaṃ tanu deva rukkhe,",
  "anya-e0607n:245": "Sākhā sakhā tava bhujā bhajataṃ dadantu.",
  "anya-e0607n:246": "49.",
  "anya-e0607n:247": "Nihāra bindu sahitaggadalopa sobhi,",
  "anya-e0607n:248": "Byālamba ratta padumadvaya bhaṅgi bhājā;",
  "anya-e0607n:249": "Pāpārisīsalunateniva ratta rattā,",
  "anya-e0607n:250": "Rattā karā tava bhavumbhuvi maṅgalāya.",
  "anya-e0607n:251": "50.",
  "anya-e0607n:252": "Rupassirī carita caṅkama vibbhamā te,",
  "anya-e0607n:253": "Piṭṭhī yathā kalala muddhani setu bhūtā;",
  "anya-e0607n:254": "Evaṃ bhavaṇṇava samuttaraṇāya setu,",
  "anya-e0607n:255": "Hotammahākanaka saṃkama sannikāsā.",
  "anya-e0607n:256": "51.",
  "anya-e0607n:257": "Saddhamma desana manohara bherināda,",
  "anya-e0607n:258": "Saṃcāraṇe sivapuraṃ visituṃ janānaṃ;",
  "anya-e0607n:259": "Gīvā suvaṇṇamaya cāru mutiṅga bheri,",
  "anya-e0607n:260": "Bhāvambhajā bhavatu bhūta vibhūtiyā te.",
  "anya-e0607n:261": "52.",
  "anya-e0607n:262": "Lakhī nivāsa vadanambuja matta ninna,",
  "anya-e0607n:263": "Mākaḍḍhayaṃ jana vilocana cañcarīke;",
  "anya-e0607n:264": "Sorabbha dhamma makaranda nisandamānaṃ,",
  "anya-e0607n:265": "Piṇetu tena sarasena sabhā jane te.",
  "anya-e0607n:266": "53.",
  "anya-e0607n:267": "Lakhī samāruhita vattarathe rathaṅga,",
  "anya-e0607n:268": "Dvandānu kāri miga rāja kapola līlaṃ;",
  "anya-e0607n:269": "Tādaṅka maṇḍalayugaṃ viya kaṇṇabhājaṃ,",
  "anya-e0607n:270": "Gaṇḍatthaladvyamalaṃkurutaṃ janatte.",
  "anya-e0607n:271": "54.",
  "anya-e0607n:272": "Lāvaṇṇa maṇṇava pavāḷa latā dvayābhaṃ,",
  "anya-e0607n:273": "Tandeha deva taru pallava kante mantaṃ;",
  "anya-e0607n:274": "Vattāravinda makaranda parājisobhaṃ,",
  "anya-e0607n:275": "Rattādharadvayamadho kurutaṃ janāghaṃ.",
  "anya-e0607n:276": "55.",
  "anya-e0607n:277": "Uṇṇā sakuntigata matthaka natthu kūpa,",
  "anya-e0607n:278": "Subbhū lakāra sahitoṭṭha pavāḷa nāvā;",
  "anya-e0607n:279": "Gattuttararaṇṇava gatā tava jantukānaṃ,",
  "anya-e0607n:280": "Hotaṃ bhavaṇṇava samuttaranaya nātha.",
  "anya-e0607n:281": "56.",
  "anya-e0607n:282": "Isaṃ vikāsa padumodara kesarāli,",
  "anya-e0607n:283": "Līlā vinaddha rucirā tava danta panti;",
  "anya-e0607n:284": "Vānī vadhū dharita mālati mālya tulyā,",
  "anya-e0607n:285": "Tassaṃ jānassa manarañjana mācareyya.",
  "anya-e0607n:286": "57.",
  "anya-e0607n:287": "Saddhamma nijjhara suratta silātalābhā,",
  "anya-e0607n:288": "Jivhā vacī naṭa vadhū kala raṅga bhūtā;",
  "anya-e0607n:289": "Saddhamma seṭṭha taraṇī nihitappiyā te,",
  "anya-e0607n:290": "Saṃsāra sāgara samuttaraṇāya hotu.",
  "anya-e0607n:291": "58.",
  "anya-e0607n:292": "Dantaṃsu kañcukīta rattadharo padhāne,",
  "anya-e0607n:293": "Jivhā suratta sayane mukha mandiraṭṭhe;",
  "anya-e0607n:294": "Āmokkha mutti vadhuyā sayitāya tuyhaṃ,",
  "anya-e0607n:295": "Kubbantu saṃgama malaṃ jana sotu kāmi.",
  "anya-e0607n:296": "59.",
  "anya-e0607n:297": "Uṇṇā tathābhinava patta varābhi rāmā,",
  "anya-e0607n:298": "Līlollasanta bhamukadvaya nīla pattā;",
  "anya-e0607n:299": "Ghānoru cāru kadalī vadanā lavālā,",
  "anya-e0607n:300": "Tuyhaṃ pavattatu ciraṃ jana maṅgalāya.",
  "anya-e0607n:301": "60.",
  "anya-e0607n:302": "Bālatthalī hari silātala piṭṭhikaṭṭha,",
  "anya-e0607n:303": "Bhūvallaridvaya mayūra yugassa tuyhaṃ;",
  "anya-e0607n:304": "Pañcappabhā rucira piccha yugassirīkaṃ,",
  "anya-e0607n:305": "Nettadvayaṃ manasi puñchatu pāpadhūliṃ.",
  "anya-e0607n:306": "61.",
  "anya-e0607n:307": "Indīvarāntagata bhiṅgika panti bhaṅgi,",
  "anya-e0607n:308": "Pañambujassaratate viya gacchapantī;",
  "anya-e0607n:309": "Nettambujassiri tirokaraṇīva tuyhaṃ,",
  "anya-e0607n:310": "Pamhāvalī sirigateha tiro karontu.",
  "anya-e0607n:311": "62.",
  "anya-e0607n:312": "Vattullasambuja vilocana haṃsa tuṇḍa,",
  "anya-e0607n:313": "Kañjaṃsu piñjara mulāla latā dvyābhaṃ;",
  "anya-e0607n:314": "Dolādvayaṃva savaṇadvyamatta lakkhyā,",
  "anya-e0607n:315": "Hotaṃ tavajja janatā maticārahetu.",
  "anya-e0607n:316": "63.",
  "anya-e0607n:317": "Vammīka matthaka sayānaka bhūridatta,",
  "anya-e0607n:318": "Bhoginda bhogavali vibbhamamā vahanti;",
  "anya-e0607n:319": "Ghānopariṭṭhitamune tava tuṇṇamuṇṇā,",
  "anya-e0607n:320": "Taggāhino viya janassa dadātu vittaṃ.",
  "anya-e0607n:321": "64.",
  "anya-e0607n:322": "Rūpindirāya vijaye khila loka rūpaṃ,",
  "anya-e0607n:323": "Ghāṇoru cāru parighopari baddha siddhā;",
  "anya-e0607n:324": "Nīlābha vāta viluthanta vayaddhajābhā,",
  "anya-e0607n:325": "Tiṭṭhantu sajja duritāri jayāya te bhū.",
  "anya-e0607n:326": "65.",
  "anya-e0607n:327": "Uṇṇassitopala nivesita bunda sandhi,",
  "anya-e0607n:328": "Ghāṇoru piṇḍakamaghā tapa rundhitunte;",
  "anya-e0607n:329": "Hotammukhambuja sirī sirasussitābhaṃ,",
  "anya-e0607n:330": "Bhū nīla paṭṭika lalāta suvaṇṇa chattaṃ.",
  "anya-e0607n:331": "66.",
  "anya-e0607n:332": "Rupaṅka vedana vilocana bāna diṭṭhī,",
  "anya-e0607n:333": "Dhārā nisāna maṇivaṭṭa sirī siro te;",
  "anya-e0607n:334": "Siddhā matosadha katañjana puñja lakkhī,",
  "anya-e0607n:335": "Hotaṃ janassa nayanāmaya nāsanāya.",
  "anya-e0607n:336": "67.",
  "anya-e0607n:337": "Sakkhandha bāhuyuga torana majjha gīvā,",
  "anya-e0607n:338": "Dharappitassirighato pari mussavāya;",
  "anya-e0607n:339": "Nīluppalāva ṭhapitā savibhatti kante,",
  "anya-e0607n:340": "Kesā bhavantu bhuvanattaya maṅgalāya.",
  "anya-e0607n:341": "68.",
  "anya-e0607n:342": "Hemagghiye ṭhapita nīla silā kapāle,",
  "anya-e0607n:343": "Pajjota jāla lalitaṃ muni sārayantī;",
  "anya-e0607n:344": "Rūpassirī sirasi bhūsita hema mālā,",
  "anya-e0607n:345": "Kārā karotu subhagaṃ tava ketu mālā.",
  "anya-e0607n:346": "69.",
  "anya-e0607n:347": "Bhyāmappabhāli tava kañcana mora kāle,",
  "anya-e0607n:348": "Surodaye vitata candaka cakkalakkhī;",
  "anya-e0607n:349": "Meghāvanaddha sikharunnata hema selā,",
  "anya-e0607n:350": "Yantindacāpa vikatīva dadātu sobhaṃ.",
  "anya-e0607n:351": "70.",
  "anya-e0607n:352": "Paṭṭhāya te paṇidhito suci dāna sīla,",
  "anya-e0607n:353": "Nekkhamma pañña viriyakkhama saccadhiṭṭhā;",
  "anya-e0607n:354": "Mettā upekkhiti ime dasa pūratova,",
  "anya-e0607n:355": "Pūrentu pārami guṇā janatānamatte.",
  "anya-e0607n:356": "71.",
  "anya-e0607n:357": "Pattuttaruttaradasā paṇidhāna bījā,",
  "anya-e0607n:358": "Cetoradharāya karuṇā jala sekha vuddhā;",
  "anya-e0607n:359": "Sabbaññu ñāṇa phaladā sati vāṭa guttā,",
  "anya-e0607n:360": "Taṃ samphalandisatu pāramitā latā te.",
  "anya-e0607n:361": "72.",
  "anya-e0607n:362": "Ābodhi puṇṇami padiṭṭha dinādito te,",
  "anya-e0607n:363": "Sambhāra kāla sita pakkha kamābhi vuddho;",
  "anya-e0607n:364": "Sampuṇṇa pārami guṇāmataraṃsi taṃva,",
  "anya-e0607n:365": "Sabbaṅgi kunda kumudāni pabodhayeyya.",
  "anya-e0607n:366": "73.",
  "anya-e0607n:367": "Āpacchimabbhava sivapphala lābha dānā,",
  "anya-e0607n:368": "Dānappabandhamapidāna phalappabhandaṃ;",
  "anya-e0607n:369": "Saṃvaḍḍhayi tvaṃ abhipatthanato yathevaṃ,",
  "anya-e0607n:370": "Jantuttaruttara phalaṃ khalu sambhunantu.",
  "anya-e0607n:371": "74.",
  "anya-e0607n:372": "Ārambhatoppabhuti yāva tavaggamaggā,",
  "anya-e0607n:373": "Vikkhālita ghakalusaṃ suci sīla toyaṃ;",
  "anya-e0607n:374": "Mettā dayā madhura sītalatāyupetaṃ,",
  "anya-e0607n:375": "Sodhetu tvaṃva bhava nissita jantu metaṃ.",
  "anya-e0607n:376": "75.",
  "anya-e0607n:377": "Āpaccimattamabhinikkhamanābhiyogā,",
  "anya-e0607n:378": "Paṭṭhāya tampabhavato paripuṇṇa gehā;",
  "anya-e0607n:379": "Tvaṃ sabba jāti gahato api nikkhamittho,",
  "anya-e0607n:380": "Evaṃ janā bhava dukhā khalu nikkhamantu.",
  "anya-e0607n:381": "76.",
  "anya-e0607n:382": "Ekaggato pala tale nisitā cirandhi,",
  "anya-e0607n:383": "Dhārā sucittu sutale sati daṇḍa baddhe;",
  "anya-e0607n:384": "Nibbijjhi lakkhaṇa dhanuṭṭhiti santi lakkhaṃ,",
  "anya-e0607n:385": "Khittā tayonamanu vijjhatu jantu khittā.",
  "anya-e0607n:386": "77.",
  "anya-e0607n:387": "Tvaṃ pāramī jala nidhiṃ caturiha bāhu,",
  "anya-e0607n:388": "Sattīhi suttari ciraṃ janakova sindhuṃ;",
  "anya-e0607n:389": "Sampanna vikkama phalosi yathā casova,",
  "anya-e0607n:390": "Evaṃ janā viriyatapphalame dhayantu.",
  "anya-e0607n:391": "78.",
  "anya-e0607n:392": "Satta paradha dahanesu ciraṃ sudhantaṃ,",
  "anya-e0607n:393": "Khantī suvaṇṇa kata rūpa samantimattā;",
  "anya-e0607n:394": "Sabbā parādhamasahi tvaṃasayhamevaṃ,",
  "anya-e0607n:395": "Sabbe janāpi khamanena bhajantu santiṃ.",
  "anya-e0607n:396": "79.",
  "anya-e0607n:397": "Lakkhādhikaṃ catura saṃkhiya kappa kālaṃ,",
  "anya-e0607n:398": "Saccena suṭṭhu paribhāvita vācino te;",
  "anya-e0607n:399": "Vācāya sacca phusitāya samenti jantu,",
  "anya-e0607n:400": "Evaṃ visuddha vacanā janatā bhavantu.",
  "anya-e0607n:401": "80.",
  "anya-e0607n:402": "Ādinna dhamma mahiyatthira suppatiṭṭhā,",
  "anya-e0607n:403": "Dhiṭṭhāna pārami mahā vajiraddi tuyhaṃ;",
  "anya-e0607n:404": "Sattena kena pi yathāhi abhejja nejjo,",
  "anya-e0607n:405": "Evaṃ janāpi kusalesu adhiṭṭha hantu.",
  "anya-e0607n:406": "81.",
  "anya-e0607n:407": "Tvaṃ sabba satta cirabhāvita metta citta,",
  "anya-e0607n:408": "Toyehi saṃsamita kodha mahā hutāso;",
  "anya-e0607n:409": "Lokuttaraṃ taditaraṃ hitamāvahittho,",
  "anya-e0607n:410": "Evaṃ janesu janatā hitamāvahantū.",
  "anya-e0607n:411": "82.",
  "anya-e0607n:412": "Mittopakāra paṭipakkha janāpakāre,",
  "anya-e0607n:413": "Tvaṃ nibbikāra manaso cirabhāvanāya;",
  "anya-e0607n:414": "Pattosilābha pabhutaṭṭusu nibbikāraṃ,",
  "anya-e0607n:415": "Evaṃ janānunaya kopa nudā bhavantu.",
  "anya-e0607n:416": "83.",
  "anya-e0607n:417": "Sampanna hetu vibhavo tusite vimānaṃ,",
  "anya-e0607n:418": "Yuttaṃ guṇehi navabhippadavī vimānaṃ;",
  "anya-e0607n:419": "Tvaṃ vādhiparamidhirohiniyā tiloko,",
  "anya-e0607n:420": "Ārohatu bhaya sukhaṃ padavī vimānaṃ.",
  "anya-e0607n:421": "84.",
  "anya-e0607n:422": "Tvaṃverahaṃsi samabujjhi yathāca sammā,",
  "anya-e0607n:423": "Sampanna vijja caraṇo sugatosi hontu;",
  "anya-e0607n:424": "Lokaṃ vido purisadammasusārathī si,",
  "anya-e0607n:425": "Satthāsi bujjhi bhagavā si tatheva jantu.",
  "anya-e0607n:426": "85.",
  "anya-e0607n:427": "Saccitta bhū nidahitaṃ janatāya tuyhaṃ,",
  "anya-e0607n:428": "Kalyāṇavaṇṇaratanaṇṇavajātibhinnaṃ;",
  "anya-e0607n:429": "Dukkhaggi cora jalupaddutajāti gehe,",
  "anya-e0607n:430": "Tassā sukhaṃ bhavatu jīvitumāpadāya.",
  "anya-e0607n:431": "86.",
  "anya-e0607n:432": "Vācā vicitta vara tantu gataṅgi kaṇṭhe,",
  "anya-e0607n:433": "Svā mutta sagguṇa mahā ratanā valī te;",
  "anya-e0607n:434": "Vevaṇṇi yattani bhavaṃ sakalampahāya,",
  "anya-e0607n:435": "Hotañjanassa siri saṅgama maṅgalāya.",
  "anya-e0607n:436": "87.",
  "anya-e0607n:437": "Taṃ sagguṇatthava dahaṭṭha sutippanāli,",
  "anya-e0607n:438": "Nissandamāna guṇanīra nipāna tinte;",
  "anya-e0607n:439": "Khettetta saññini janā kata loma haṃsa,",
  "anya-e0607n:440": "Bījankurī kusala sassa phalaṃ labhantu.",
  "anya-e0607n:441": "88.",
  "anya-e0607n:442": "Āpāyikappabhuti dukkha nidāgha kāla,",
  "anya-e0607n:443": "Santāpitā nikhila loka mano kadambā;",
  "anya-e0607n:444": "Taṃ vaṅṇa megha phusanā hasanaṅkurehi,",
  "anya-e0607n:445": "Iddhā bhavantu mati vallari vellītā te.",
  "anya-e0607n:446": "89.",
  "anya-e0607n:447": "Hetuddasā phaladasā samavaṭṭhitaṃ taṃ,",
  "anya-e0607n:448": "Sabbattha satta hitamāvahaṇena siddhaṃ;",
  "anya-e0607n:449": "Cintāpathātiganubhāva vibhāvanante,",
  "anya-e0607n:450": "Bhūtānamatthu caritabbhutamattha sidhyā.",
  "anya-e0607n:451": "90.",
  "anya-e0607n:452": "Aṅgārakāsumabhilaṅghiya dāna kāle,",
  "anya-e0607n:453": "Bhattattano pada paṭicchaka paṅkajā ca;",
  "anya-e0607n:454": "Yātakkhaṇe tava pade dhaṭamuṭṭhahitvā,",
  "anya-e0607n:455": "Paṅkeruhāṃ siva madhuṃ sarataṃ dadantu.",
  "anya-e0607n:456": "91.",
  "anya-e0607n:457": "Saccena maccha pati vassita vassadhārā,",
  "anya-e0607n:458": "Satte dayāya tava vassita vassadhārā;",
  "anya-e0607n:459": "Gimhe janassa samayiṃsu yathā tathātā,",
  "anya-e0607n:460": "Dhamambuvuṭṭhiva samentu kilesa dāhe.",
  "anya-e0607n:461": "92.",
  "anya-e0607n:462": "Chaddanta nāga patinā khamatā parādhaṃ,",
  "anya-e0607n:463": "Chetvā kare ṭhapita dantavarāva luddaṃ;",
  "anya-e0607n:464": "Loke hitāya ṭhapitā tava danta dhātu,",
  "anya-e0607n:465": "Seṭṭhā janaṃ siva puraṃ lahu pāpayantu.",
  "anya-e0607n:466": "93.",
  "anya-e0607n:467": "Taṃ temiyākhya yatinossama mālakamhi,",
  "anya-e0607n:468": "Okiṇṇa mutta kanakā vuja vippakiṇṇā;",
  "anya-e0607n:469": "Kāruñña vārida cuto daka bindu bandhū,",
  "anya-e0607n:470": "Dhātu samentu tava jantusu dukkhadāhe.",
  "anya-e0607n:471": "94.",
  "anya-e0607n:472": "Raṭṭhassa attha caraṇāya asammukhassa,",
  "anya-e0607n:473": "Rāmena dinna tiṇa saṃkhata pādukāva;",
  "anya-e0607n:474": "Bhuttā tayā ciramasammukha nāgatassa,",
  "anya-e0607n:475": "Lokassa atthamanu tiṭṭhatu patta dhātu.",
  "anya-e0607n:476": "95.",
  "anya-e0607n:477": "Vutto janānamupadissa varāha raññā,",
  "anya-e0607n:478": "Satthiṃ sahassa saradaṃ viya ñāya dhammo;",
  "anya-e0607n:479": "Ādeyya heyyamupadissa tayā pavutto,",
  "anya-e0607n:480": "Dhammo pavattatu ciraṃ janatā hitāya.",
  "anya-e0607n:481": "96.",
  "anya-e0607n:482": "Mārāri maddana hitādhigamaṃ karotā,",
  "anya-e0607n:483": "Bhatto tayā vara mahā jaya bodhi rājā;",
  "anya-e0607n:484": "Saggā pavavagga hitahetu janassa hantvā,",
  "anya-e0607n:485": "Sabbantarāyamiha tiṭṭhatu suṭṭhu sajjo.",
  "anya-e0607n:486": "97.",
  "anya-e0607n:487": "Sāmoda vaṇṇa bhajanī guṇa mañjarīyaṃ,",
  "anya-e0607n:488": "Cariyā latā vikasitā tava sapphalaṅgaṃ;",
  "anya-e0607n:489": "Okiṇṇa citta madhupe rasa pīṇayanti,",
  "anya-e0607n:490": "Sambhāvitā bhuvi pavattatu matthakehi.",
  "anya-e0607n:491": "98.",
  "anya-e0607n:492": "Sambuddha selavalayantara jānanavhā,",
  "anya-e0607n:493": "Nottattato tipathagā yati sāgaraṭṭhā;",
  "anya-e0607n:494": "Dhammā pagā suti vase tarite puṇanti,",
  "anya-e0607n:495": "Sambhāra sassamiha vattatu pacayanti.",
  "anya-e0607n:496": "99.",
  "anya-e0607n:497": "Paññāṇa kūpa sita paggaha vāyu gāhī,",
  "anya-e0607n:498": "Saddhā lakāra sahitā sati pota vāhā;",
  "anya-e0607n:499": "Sampāpayātu bhava sāgara pāra tīra,",
  "anya-e0607n:500": "Sappattanaṃ varadhane pati patti nāvā.",
  "anya-e0607n:501": "100.",
  "anya-e0607n:502": "Bojjhaṅga satta ratanākara dhamma khandha,",
  "anya-e0607n:503": "Gambhīra nīra caya sāsana sāgaro saṃ;",
  "anya-e0607n:504": "So sīlyananta tanu veṭitha ñāṇa mantha,",
  "anya-e0607n:505": "Selena manthitavataṃ disatā mataṃ ve.",
  "anya-e0607n:506": "101.",
  "anya-e0607n:507": "Vuttena tena vidhinā vidhinā tato taṃ,",
  "anya-e0607n:508": "Laddhā nubhūtamamataṃ khila dosa nāsaṃ;",
  "anya-e0607n:509": "Accanta roga jaratā maraṇā bhi bhūtaṃ,",
  "anya-e0607n:510": "Bhūtaṃ karotu amaraṃ ajaraṃ arogaṃ.",
  "anya-e0607n:511": "102.",
  "anya-e0607n:512": "Saddhamma rāja raviniggata dhammaraṃsi,",
  "anya-e0607n:513": "Phullo dhutaṅgadala saṃvara kesarāli;",
  "anya-e0607n:514": "Saṅghāravinda nikaro samadhuṃ samādhi,",
  "anya-e0607n:515": "Sakkiṇṇiko disatu sāsana vāpi jato.",
  "anya-e0607n:516": "103.",
  "anya-e0607n:517": "Ānanda rañña ratanādi mahā yatinda,",
  "anya-e0607n:518": "Niccappabuddha padumappiya sevinaṅgī;",
  "anya-e0607n:519": "Buddhappiyena ghana buddha guṇappiyena,",
  "anya-e0607n:520": "Therālinā racita pajjamadhuṃ pibantu.",
  "anya-e0607n:521": "104.",
  "anya-e0607n:522": "Itthaṃ rūpa guṇānukittanavasā taṃ taṃ hitā siṃ sato,",
  "anya-e0607n:523": "Vatthānussati vattita iha yathā sattesu mettā ca me;",
  "anya-e0607n:524": "Evaṃ tābhi bhavanta ruttara tarā vattantu tā bodhi me,",
  "anya-e0607n:525": "Saṃyogoca dhanehi santihi bhave kalyāna mittehi ca."
}