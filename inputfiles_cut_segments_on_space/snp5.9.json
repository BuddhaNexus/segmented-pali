{
  "snp5.9:0.1": "Sutta Nipāta 5.9 ",
  "snp5.9:0.2": "Hemakamāṇavapucchā ",
  "snp5.9:1.1": "“Ye me pubbe viyākaṁsu, ",
  "snp5.9:1.2": "(iccāyasmā hemako) ",
  "snp5.9:1.3": "Huraṁ gotamasāsanā; ",
  "snp5.9:1.4": "Iccāsi iti bhavissati, ",
  "snp5.9:1.5": "Sabbaṁ taṁ itihītihaṁ; ",
  "snp5.9:1.6": "Sabbaṁ taṁ takkavaḍḍhanaṁ, ",
  "snp5.9:1.7": "Nāhaṁ tattha abhiramiṁ. ",
  "snp5.9:2.1": "Tvañca me dhammamakkhāhi, ",
  "snp5.9:2.2": "taṇhānigghātanaṁ muni; ",
  "snp5.9:2.3": "Yaṁ viditvā sato caraṁ, ",
  "snp5.9:2.4": "tare loke visattikaṁ”. ",
  "snp5.9:3.1": "“Idha diṭṭhasutamutaviññātesu, ",
  "snp5.9:3.2": "Piyarūpesu hemaka; ",
  "snp5.9:3.3": "Chandarāgavinodanaṁ, ",
  "snp5.9:3.4": "Nibbānapadamaccutaṁ. ",
  "snp5.9:4.1": "Etadaññāya ye satā, ",
  "snp5.9:4.2": "Diṭṭhadhammābhinibbutā; ",
  "snp5.9:4.3": "Upasantā ca te sadā, ",
  "snp5.9:4.4": "Tiṇṇā loke visattikan”ti. ",
  "snp5.9:4.5": "Hemakamāṇavapucchā aṭṭhamā. "
}