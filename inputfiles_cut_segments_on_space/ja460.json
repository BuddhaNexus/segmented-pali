{
  "ja460:0.1": "Jātaka ",
  "ja460:0.2": "Ekādasakanipāta ",
  "ja460:0.3": "Mātuposakavagga ",
  "ja460:0.4": "6. Yudhañcayajātaka ",
  "ja460:1.1": "“Mittāmaccaparibyūḷhaṁ, ",
  "ja460:1.2": "ahaṁ vande rathesabhaṁ; ",
  "ja460:1.3": "Pabbajissāmahaṁ rāja, ",
  "ja460:1.4": "taṁ devo anumaññatu”. ",
  "ja460:2.1": "“Sace te ūnaṁ kāmehi, ",
  "ja460:2.2": "ahaṁ paripūrayāmi te; ",
  "ja460:2.3": "Yo taṁ hiṁsati vāremi, ",
  "ja460:2.4": "mā pabbaja yudhañcaya”. ",
  "ja460:3.1": "“Na matthi ūnaṁ kāmehi, ",
  "ja460:3.2": "hiṁsitā me na vijjati; ",
  "ja460:3.3": "Dīpañca kātumicchāmi, ",
  "ja460:3.4": "yaṁ jarā nābhikīrati”. ",
  "ja460:4.1": "“Putto vā pitaraṁ yāce, ",
  "ja460:4.2": "pitā vā puttamorasaṁ; ",
  "ja460:4.3": "Negamo taṁ yāce tāta, ",
  "ja460:4.4": "mā pabbaja yudhañcaya”. ",
  "ja460:5.1": "“Mā maṁ deva nivārehi, ",
  "ja460:5.2": "pabbajantaṁ rathesabha; ",
  "ja460:5.3": "Māhaṁ kāmehi sammatto, ",
  "ja460:5.4": "jarāya vasamanvagū”. ",
  "ja460:6.1": "“Ahaṁ taṁ tāta yācāmi, ",
  "ja460:6.2": "ahaṁ putta nivāraye; ",
  "ja460:6.3": "Ciraṁ taṁ daṭṭhumicchāmi, ",
  "ja460:6.4": "mā pabbaja yudhañcaya”. ",
  "ja460:7.1": "“Ussāvova tiṇaggamhi, ",
  "ja460:7.2": "sūriyuggamanaṁ pati; ",
  "ja460:7.3": "Evamāyu manussānaṁ, ",
  "ja460:7.4": "mā maṁ amma nivāraya”. ",
  "ja460:8.1": "“Taramāno imaṁ yānaṁ, ",
  "ja460:8.2": "āropetu rathesabha; ",
  "ja460:8.3": "Mā me mātā tarantassa, ",
  "ja460:8.4": "antarāyakarā ahu”. ",
  "ja460:9.1": "“Abhidhāvatha bhaddante, ",
  "ja460:9.2": "suññaṁ hessati rammakaṁ; ",
  "ja460:9.3": "Yudhañcayo anuññāto, ",
  "ja460:9.4": "sabbadattena rājinā. ",
  "ja460:10.1": "Yohu seṭṭho sahassassa, ",
  "ja460:10.2": "yuvā kañcanasannibho; ",
  "ja460:10.3": "Soyaṁ kumāro pabbajito, ",
  "ja460:10.4": "kāsāyavasano balī”. ",
  "ja460:11.1": "“Ubho kumārā pabbajitā, ",
  "ja460:11.2": "yudhañcayo yudhiṭṭhilo; ",
  "ja460:11.3": "Pahāya mātāpitaro, ",
  "ja460:11.4": "saṅgaṁ chetvāna maccuno”ti. ",
  "ja460:12.1": "Yudhañcayajātakaṁ chaṭṭhaṁ. "
}