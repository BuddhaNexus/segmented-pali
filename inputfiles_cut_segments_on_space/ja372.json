{
  "ja372:0.1": "Jātaka ",
  "ja372:0.2": "Pañcakanipāta ",
  "ja372:0.3": "Aḍḍhavagga ",
  "ja372:0.4": "2. Migapotakajātaka ",
  "ja372:1.1": "“Agārā paccupetassa, ",
  "ja372:1.2": "anagārassa te sato; ",
  "ja372:1.3": "Samaṇassa na taṁ sādhu, ",
  "ja372:1.4": "yaṁ petamanusocasi”. ",
  "ja372:2.1": "“Saṁvāsena have sakka, ",
  "ja372:2.2": "manussassa migassa vā; ",
  "ja372:2.3": "Hadaye jāyate pemaṁ, ",
  "ja372:2.4": "na taṁ sakkā asocituṁ”. ",
  "ja372:3.1": "“Mataṁ marissaṁ rodanti, ",
  "ja372:3.2": "ye rudanti lapanti ca; ",
  "ja372:3.3": "Tasmā tvaṁ isi mā rodi, ",
  "ja372:3.4": "roditaṁ moghamāhu santo. ",
  "ja372:4.1": "Roditena have brahme, ",
  "ja372:4.2": "mato peto samuṭṭhahe; ",
  "ja372:4.3": "Sabbe saṅgamma rodāma, ",
  "ja372:4.4": "aññamaññassa ñātake”. ",
  "ja372:5.1": "“Ādittaṁ vata maṁ santaṁ, ",
  "ja372:5.2": "ghatasittaṁva pāvakaṁ; ",
  "ja372:5.3": "Vārinā viya osiñcaṁ, ",
  "ja372:5.4": "sabbaṁ nibbāpaye daraṁ. ",
  "ja372:6.1": "Abbahi vata me sallaṁ, ",
  "ja372:6.2": "yamāsi hadayassitaṁ; ",
  "ja372:6.3": "Yo me sokaparetassa, ",
  "ja372:6.4": "puttasokaṁ apānudi. ",
  "ja372:7.1": "Sohaṁ abbūḷhasallosmi, ",
  "ja372:7.2": "vītasoko anāvilo; ",
  "ja372:7.3": "Na socāmi na rodāmi, ",
  "ja372:7.4": "tava sutvāna vāsavā”ti. ",
  "ja372:8.1": "Migapotakajātakaṁ dutiyaṁ. "
}