{
  "ja470:0.1": "Jātaka ",
  "ja470:0.2": "Dvādasakanipāta ",
  "ja470:0.3": "Cūḷakuṇālavagga ",
  "ja470:0.4": "7. Kosiyajātaka ",
  "ja470:1.1": "“Neva kiṇāmi napi vikkiṇāmi, ",
  "ja470:1.2": "Na cāpi me sannicayo ca atthi; ",
  "ja470:1.3": "Sukiccharūpaṁ vatidaṁ parittaṁ, ",
  "ja470:1.4": "Patthodano nālamayaṁ duvinnaṁ”. ",
  "ja470:2.1": "“Appamhā appakaṁ dajjā, ",
  "ja470:2.2": "anumajjhato majjhakaṁ; ",
  "ja470:2.3": "Bahumhā bahukaṁ dajjā, ",
  "ja470:2.4": "adānaṁ nupapajjati. ",
  "ja470:3.1": "Taṁ taṁ vadāmi kosiya, ",
  "ja470:3.2": "dehi dānāni bhuñja ca”. ",
  "ja470:3.3": "Ariyamaggaṁ samārūha, ",
  "ja470:3.4": "nekāsī labhate sukhaṁ. ",
  "ja470:4.1": "“Moghañcassa hutaṁ hoti, ",
  "ja470:4.2": "moghañcāpi samīhitaṁ; ",
  "ja470:4.3": "Atithismiṁ yo nisinnasmiṁ, ",
  "ja470:4.4": "eko bhuñjati bhojanaṁ. ",
  "ja470:5.1": "Taṁ taṁ vadāmi kosiya, ",
  "ja470:5.2": "dehi dānāni bhuñja ca; ",
  "ja470:5.3": "Ariyamaggaṁ samārūha, ",
  "ja470:5.4": "nekāsī labhate sukhaṁ”. ",
  "ja470:6.1": "“Saccañcassa hutaṁ hoti, ",
  "ja470:6.2": "saccañcāpi samīhitaṁ; ",
  "ja470:6.3": "Atithismiṁ yo nisinnasmiṁ, ",
  "ja470:6.4": "neko bhuñjati bhojanaṁ. ",
  "ja470:7.1": "Taṁ taṁ vadāmi kosiya, ",
  "ja470:7.2": "dehi dānāni bhuñja ca; ",
  "ja470:7.3": "Ariyamaggaṁ samārūha, ",
  "ja470:7.4": "nekāsī labhate sukhaṁ”. ",
  "ja470:8.1": "“Sarañca juhati poso, ",
  "ja470:8.2": "bahukāya gayāya ca; ",
  "ja470:8.3": "Doṇe timbarutitthasmiṁ, ",
  "ja470:8.4": "sīghasote mahāvahe. ",
  "ja470:9.1": "Atra cassa hutaṁ hoti, ",
  "ja470:9.2": "atra cassa samīhitaṁ; ",
  "ja470:9.3": "Atithismiṁ yo nisinnasmiṁ, ",
  "ja470:9.4": "neko bhuñjati bhojanaṁ. ",
  "ja470:10.1": "Taṁ taṁ vadāmi kosiya, ",
  "ja470:10.2": "dehi dānāni bhuñja ca; ",
  "ja470:10.3": "Ariyamaggaṁ samārūha, ",
  "ja470:10.4": "nekāsī labhate sukhaṁ”. ",
  "ja470:11.1": "“Baḷisañhi so nigilati, ",
  "ja470:11.2": "dīghasuttaṁ sabandhanaṁ; ",
  "ja470:11.3": "Atithismiṁ yo nisinnasmiṁ, ",
  "ja470:11.4": "eko bhuñjati bhojanaṁ. ",
  "ja470:12.1": "Taṁ taṁ vadāmi kosiya, ",
  "ja470:12.2": "dehi dānāni bhuñja ca; ",
  "ja470:12.3": "Ariyamaggaṁ samārūha, ",
  "ja470:12.4": "nekāsī labhate sukhaṁ”. ",
  "ja470:13.1": "“Uḷāravaṇṇā vata brāhmaṇā ime, ",
  "ja470:13.2": "Ayañca vo sunakho kissa hetu; ",
  "ja470:13.3": "Uccāvacaṁ vaṇṇanibhaṁ vikubbati, ",
  "ja470:13.4": "Akkhātha no brāhmaṇā ke nu tumhe”. ",
  "ja470:14.1": "“Cando ca suriyo ca ubho idhāgatā, ",
  "ja470:14.2": "Ayaṁ pana mātali devasārathi; ",
  "ja470:14.3": "Sakkohamasmi tidasānamindo, ",
  "ja470:14.4": "Eso ca kho pañcasikhoti vuccati”. ",
  "ja470:15.1": "“Pāṇissarā mudiṅgā ca, ",
  "ja470:15.2": "murajālambarāni ca; ",
  "ja470:15.3": "Suttamenaṁ pabodhenti, ",
  "ja470:15.4": "paṭibuddho ca nandati”. ",
  "ja470:16.1": "“Ye kecime maccharino kadariyā, ",
  "ja470:16.2": "Paribhāsakā samaṇabrāhmaṇānaṁ; ",
  "ja470:16.3": "Idheva nikkhippa sarīradehaṁ, ",
  "ja470:16.4": "Kāyassa bhedā nirayaṁ vajanti”. ",
  "ja470:17.1": "“Ye kecime suggatimāsamānā, ",
  "ja470:17.2": "Dhamme ṭhitā saṁyame saṁvibhāge; ",
  "ja470:17.3": "Idheva nikkhippa sarīradehaṁ, ",
  "ja470:17.4": "Kāyassa bhedā sugatiṁ vajanti. ",
  "ja470:18.1": "Tvaṁ nosi ñāti purimāsu jātisu, ",
  "ja470:18.2": "So maccharī rosako pāpadhammo; ",
  "ja470:18.3": "Taveva atthāya idhāgatamhā, ",
  "ja470:18.4": "Mā pāpadhammo nirayaṁ gamittha”. ",
  "ja470:19.1": "“Addhā hi maṁ vo hitakāmā, ",
  "ja470:19.2": "yaṁ maṁ samanusāsatha; ",
  "ja470:19.3": "Sohaṁ tathā karissāmi, ",
  "ja470:19.4": "sabbaṁ vuttaṁ hitesibhi. ",
  "ja470:20.1": "Esāhamajjeva upāramāmi, ",
  "ja470:20.2": "Na cāpihaṁ kiñci kareyya pāpaṁ; ",
  "ja470:20.3": "Na cāpi me kiñci adeyyamatthi, ",
  "ja470:20.4": "Na cāpidatvā udakaṁ pivāmi. ",
  "ja470:21.1": "Evañca me dadato sabbakālaṁ, ",
  "ja470:21.2": "Bhogā ime vāsava khīyissanti; ",
  "ja470:21.3": "Tato ahaṁ pabbajissāmi sakka, ",
  "ja470:21.4": "Hitvāna kāmāni yathodhikānī”ti. ",
  "ja470:22.1": "Kosiyajātakaṁ sattamaṁ. "
}