{
  "ja430:0.1": "Jātaka ",
  "ja430:0.2": "Navakanipāta ",
  "ja430:0.3": "Gijjhavagga ",
  "ja430:0.4": "4. Cūḷasuvajātaka ",
  "ja430:1.1": "“Santi rukkhā haripattā, ",
  "ja430:1.2": "dumā nekaphalā bahū; ",
  "ja430:1.3": "Kasmā nu sukkhe koḷāpe, ",
  "ja430:1.4": "suvassa nirato mano”. ",
  "ja430:2.1": "“Phalassa upabhuñjimhā, ",
  "ja430:2.2": "nekavassagaṇe bahū; ",
  "ja430:2.3": "Aphalampi viditvāna, ",
  "ja430:2.4": "sāva metti yathā pure”. ",
  "ja430:3.1": "“Sukkhañca rukkhaṁ koḷāpaṁ, ",
  "ja430:3.2": "opattamaphalaṁ dumaṁ; ",
  "ja430:3.3": "Ohāya sakuṇā yanti, ",
  "ja430:3.4": "kiṁ dosaṁ passase dija”. ",
  "ja430:4.1": "“Ye phalatthā sambhajanti, ",
  "ja430:4.2": "aphaloti jahanti naṁ; ",
  "ja430:4.3": "Attatthapaññā dummedhā, ",
  "ja430:4.4": "te honti pakkhapātino”. ",
  "ja430:5.1": "“Sādhu sakkhi kataṁ hoti, ",
  "ja430:5.2": "metti saṁsati santhavo; ",
  "ja430:5.3": "Sacetaṁ dhammaṁ rocesi, ",
  "ja430:5.4": "pāsaṁsosi vijānataṁ. ",
  "ja430:6.1": "So te suva varaṁ dammi, ",
  "ja430:6.2": "pattayāna vihaṅgama; ",
  "ja430:6.3": "Varaṁ varassu vakkaṅga, ",
  "ja430:6.4": "yaṁ kiñci manasicchasi”. ",
  "ja430:7.1": "“Api nāma naṁ passeyyaṁ, ",
  "ja430:7.2": "sapattaṁ saphalaṁ dumaṁ; ",
  "ja430:7.3": "Daliddova nidhiṁ laddhā, ",
  "ja430:7.4": "nandeyyāhaṁ punappunaṁ”. ",
  "ja430:8.1": "“Tato amatamādāya, ",
  "ja430:8.2": "abhisiñci mahīruhaṁ; ",
  "ja430:8.3": "Tassa sākhā virūhiṁsu, ",
  "ja430:8.4": "sītacchāyā manoramā”. ",
  "ja430:9.1": "“Evaṁ sakka sukhī hohi, ",
  "ja430:9.2": "saha sabbehi ñātibhi; ",
  "ja430:9.3": "Yathāhamajja sukhito, ",
  "ja430:9.4": "disvāna saphalaṁ dumaṁ. ",
  "ja430:10.1": "Suvassa ca varaṁ datvā, ",
  "ja430:10.2": "katvāna saphalaṁ dumaṁ; ",
  "ja430:10.3": "Pakkāmi saha bhariyāya, ",
  "ja430:10.4": "devānaṁ nandanaṁ vanan”ti. ",
  "ja430:11.1": "Cūḷasuvajātakaṁ catutthaṁ. "
}