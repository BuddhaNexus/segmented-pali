{
  "anya-e1009n5:0": "Abhiyācaka",
  "anya-e1009n5:1": "4.",
  "anya-e1009n5:2": "Dhamma-saṅghaṁ vanditvāna,",
  "anya-e1009n5:3": "Sabba-lokassa nāyakaṁ;",
  "anya-e1009n5:4": "Yācito tikakrapa-therena,",
  "anya-e1009n5:5": "Māṇavena ca dhīmatā.",
  "anya-e1009n5:6": "5.",
  "anya-e1009n5:7": "Uttānameva saṅkhepaṁ,",
  "anya-e1009n5:8": "Nānā-sattha-sudhāritaṁ;",
  "anya-e1009n5:9": "Nara-dakkhaṁ likhissami,",
  "anya-e1009n5:10": "Passantu dhīra-māmakā.",
  "anya-e1009n5:11": "6.",
  "anya-e1009n5:12": "Kosajjaṁ bhayato disvā,",
  "anya-e1009n5:13": "Vīriyañjāpi khemato;",
  "anya-e1009n5:14": "Āraddhavīriyā hotha;",
  "anya-e1009n5:15": "Esā buddhānusāsanī.",
  "anya-e1009n5:16": "7.",
  "anya-e1009n5:17": "Vīriyavā kho bhikkhave ariyasāvako,",
  "anya-e1009n5:18": "Akusalaṁ pajahati, kusalaṁ bhāveti;",
  "anya-e1009n5:19": "Sāvajjaṁ pajahati, anavajjaṁ bhāveti,",
  "anya-e1009n5:20": "Suddhamattānaṁ pariharatīti.",
  "anya-e1009n5:21": "Vīriyavato kiṁ nāma kammaṁ na sijjhati;",
  "anya-e1009n5:22": "Purisakāro nāma na nassati,",
  "anya-e1009n5:23": "Sukhe patiṭṭhāpetīti jānāmi.",
  "anya-e1009n5:24": "8.",
  "anya-e1009n5:25": "Yathā khittaṁ nabhe leḍḍu,",
  "anya-e1009n5:26": "Dhuvaṁ patati bhūmiyaṁ;",
  "anya-e1009n5:27": "Tatheva buddha-seṭṭhānaṁ,",
  "anya-e1009n5:28": "Vacanaṁ dhuva-sassataṁ.",
  "anya-e1009n5:29": "9.",
  "anya-e1009n5:30": "A-dvejjhavacanā buddhā,",
  "anya-e1009n5:31": "A-moghavacanā jinā.",
  "anya-e1009n5:32": "10.",
  "anya-e1009n5:33": "Sussusā labhate paññaṁ,",
  "anya-e1009n5:34": "Uṭṭhātā vindate dhanaṁ;",
  "anya-e1009n5:35": "Tasmā pāḷiṁ guruṁ katvā,",
  "anya-e1009n5:36": "Imaṁ passāhi sobhaṇaṁ.",
  "anya-e1009n5:37": "11.",
  "anya-e1009n5:38": "Sussusā suta-buddhinī,",
  "anya-e1009n5:39": "Sutaṁ paññāya vaḍḍhanaṁ;",
  "anya-e1009n5:40": "Paññāya atthaṁ jānāti,",
  "anya-e1009n5:41": "Ñāto attho sukhāvaho.",
  "anya-e1009n5:42": "12.",
  "anya-e1009n5:43": "Satataṁjjhāyanaṁ vāda,",
  "anya-e1009n5:44": "Para-tantavalokanaṁ;",
  "anya-e1009n5:45": "Sabbijjācera-sevāca,",
  "anya-e1009n5:46": "Buddhi-mati-karo guṇo.",
  "anya-e1009n5:47": "13.",
  "anya-e1009n5:48": "Ati-dīghova nīgho hi,",
  "anya-e1009n5:49": "Kusīto hīna-vīriyo;",
  "anya-e1009n5:50": "Tasmā vīriyaṁ katvāna,",
  "anya-e1009n5:51": "Vijjaṁ esantu sādhavo.",
  "anya-e1009n5:52": "14.",
  "anya-e1009n5:53": "Suporiso tāva sippaṁ,",
  "anya-e1009n5:54": "Uggaṇheyya paraṁ dhanaṁ;",
  "anya-e1009n5:55": "Gaveseyya tato mantaṁ,",
  "anya-e1009n5:56": "Katheyya sacca-bhāsitaṁ.",
  "anya-e1009n5:57": "15.",
  "anya-e1009n5:58": "Pathamaṁ na parājaye sippaṁ,",
  "anya-e1009n5:59": "Dutīyaṁ na parājaye dhanaṁ;",
  "anya-e1009n5:60": "Tatīyaṁ na parājaye dhanaṁ,",
  "anya-e1009n5:61": "Catutthamatthaṁ kiṁ karissati.",
  "anya-e1009n5:62": "16.",
  "anya-e1009n5:63": "Sobhanti a-milātāni,",
  "anya-e1009n5:64": "Pupphāniva pilandhituṁ;",
  "anya-e1009n5:65": "Tathā sobhanti dārakā,",
  "anya-e1009n5:66": "Yobbaneyeva sikkhituṁ.",
  "anya-e1009n5:67": "17.",
  "anya-e1009n5:68": "Tasmā have guṇādhāraṁ,",
  "anya-e1009n5:69": "Paññā-vaḍḍhanamuttamaṁ;",
  "anya-e1009n5:70": "Sikkheyya matimā poso,",
  "anya-e1009n5:71": "Patthento hitamattano.",
  "anya-e1009n5:72": "18.",
  "anya-e1009n5:73": "Alaṁ vāyamituṁ sippe,",
  "anya-e1009n5:74": "Attha-kāmena jantunā;",
  "anya-e1009n5:75": "Kataṁ vijaññā vijjādi,",
  "anya-e1009n5:76": "Vayo te mā upajjhagā.",
  "anya-e1009n5:77": "19.",
  "anya-e1009n5:78": "Vijjaṁ sikkhe, care sīlaṁ,",
  "anya-e1009n5:79": "Dhīrena saha saṁvase;",
  "anya-e1009n5:80": "Dhanācaye, kare kammaṁ,",
  "anya-e1009n5:81": "Piyaṁ vācañca saṁvade.",
  "anya-e1009n5:82": "20.",
  "anya-e1009n5:83": "Na tveva supituṁ hoti,",
  "anya-e1009n5:84": "Ratti nakkhatta-mālinī;",
  "anya-e1009n5:85": "Paṭijaggitumevesā,",
  "anya-e1009n5:86": "Ratti hoti vijānataṁ.",
  "anya-e1009n5:87": "21.",
  "anya-e1009n5:88": "Uṭṭhāhatha nisīdatha,",
  "anya-e1009n5:89": "Ko attho supitena vo;",
  "anya-e1009n5:90": "Sādhu kho sippa-vijjāhvā,",
  "anya-e1009n5:91": "Vijjaṁ sikkhatha santataṁ.",
  "anya-e1009n5:92": "22.",
  "anya-e1009n5:93": "Ārabbhatha sadā puttā,",
  "anya-e1009n5:94": "Bahussutaṁ gavesituṁ;",
  "anya-e1009n5:95": "Yasmā loke sippavantā,",
  "anya-e1009n5:96": "Sabbā-disāsu pākaṭā.",
  "anya-e1009n5:97": "23.",
  "anya-e1009n5:98": "Sakyarūpaṁ pure santaṁ,",
  "anya-e1009n5:99": "Mayā sippaṁ na sikkhitaṁ;",
  "anya-e1009n5:100": "Kicchā vutti a-sippassa,",
  "anya-e1009n5:101": "Iti pacchā nutappati.",
  "anya-e1009n5:102": "24.",
  "anya-e1009n5:103": "Lokatthaṁ loka-kammantaṁ,",
  "anya-e1009n5:104": "Icchanto pariyesituṁ;",
  "anya-e1009n5:105": "Niccameva vīriyañca,",
  "anya-e1009n5:106": "Atthaṁ mantañja cintaye.",
  "anya-e1009n5:107": "25.",
  "anya-e1009n5:108": "Dhanavā guṇavā loke,",
  "anya-e1009n5:109": "Sabbā-disāya pākaṭo;",
  "anya-e1009n5:110": "Sīlavā paññavā macco,",
  "anya-e1009n5:111": "Sabba-lokehi pūjito.",
  "anya-e1009n5:112": "26.",
  "anya-e1009n5:113": "Sajīvati yaso yassa,",
  "anya-e1009n5:114": "Kitti yassa sajīvati;",
  "anya-e1009n5:115": "Yasa-kitti vihīnassa,",
  "anya-e1009n5:116": "Jīvantopi matopamā.",
  "anya-e1009n5:117": "27.",
  "anya-e1009n5:118": "Saddhīdha vittaṁ purisassa seṭṭhaṁ,",
  "anya-e1009n5:119": "Dhammo suciṇṇo sukhamāvahāti;",
  "anya-e1009n5:120": "Saccaṁ have sādutaraṁ rasānaṁ,",
  "anya-e1009n5:121": "Paññājīviṁ jīvitamāhu seṭṭhanti.",
  "anya-e1009n5:122": "28.",
  "anya-e1009n5:123": "Satimato sadā bhaddaṁ,",
  "anya-e1009n5:124": "Satimā sukhamedhati;",
  "anya-e1009n5:125": "Satimato suve seyyo,",
  "anya-e1009n5:126": "Verā ca parimuccati.",
  "anya-e1009n5:127": "29.",
  "anya-e1009n5:128": "Mā vo khaṇaṁ virādhetha,",
  "anya-e1009n5:129": "Khaṇātītā hi socare;",
  "anya-e1009n5:130": "Sadatthe vāyameyyātha,",
  "anya-e1009n5:131": "Khaṇo vo paṭipādito.",
  "anya-e1009n5:132": "30.",
  "anya-e1009n5:133": "Yathicchitaṁ na pappoti,",
  "anya-e1009n5:134": "A-phiyo nāvikoṇṇave;",
  "anya-e1009n5:135": "Tathevāvīriyopettha,",
  "anya-e1009n5:136": "Tasmārabheyya sāsanaṁ.",
  "anya-e1009n5:137": "31.",
  "anya-e1009n5:138": "Vāyametheva puriso,",
  "anya-e1009n5:139": "Yāva atthassa nipphadā;",
  "anya-e1009n5:140": "Nipphannasobhaṇo attho,",
  "anya-e1009n5:141": "Khantā bhiyyo na vijjati.",
  "anya-e1009n5:142": "32.",
  "anya-e1009n5:143": "Sameva ñāṇa-vāyāme,",
  "anya-e1009n5:144": "Sukhāvaho su-maṅgalo;",
  "anya-e1009n5:145": "Ñunedhike tathā no hi,",
  "anya-e1009n5:146": "Dvayena sādhu sampadā.",
  "anya-e1009n5:147": "33.",
  "anya-e1009n5:148": "Kāya-kammāni sijjhanti,",
  "anya-e1009n5:149": "Vacī-kammāni vīriyaṁ;",
  "anya-e1009n5:150": "Na hi kiccāni cintāhi,",
  "anya-e1009n5:151": "Kareyyāthīdha vāyamaṁ.",
  "anya-e1009n5:152": "34.",
  "anya-e1009n5:153": "Paṭikacceva kareyya,",
  "anya-e1009n5:154": "Taṁ jaññā hitamattano;",
  "anya-e1009n5:155": "Na sākaṭikacintāya,",
  "anya-e1009n5:156": "Mandā dhīro parakkame.",
  "anya-e1009n5:157": "35.",
  "anya-e1009n5:158": "Thirena saka-kammena,",
  "anya-e1009n5:159": "Vaḍḍhatiyeva saṁ phalaṁ;",
  "anya-e1009n5:160": "A-thirena alasena,",
  "anya-e1009n5:161": "Kara-kammaṁ phalañca no.",
  "anya-e1009n5:162": "36.",
  "anya-e1009n5:163": "Āsīsetheva puriso,",
  "anya-e1009n5:164": "Na nibbindeyya paṇḍito;",
  "anya-e1009n5:165": "Passāmi vohaṁ attānaṁ,",
  "anya-e1009n5:166": "Yathā icchiṁ tathā ahu.",
  "anya-e1009n5:167": "37.",
  "anya-e1009n5:168": "Aṇaṇo ñātīnaṁ hoti,",
  "anya-e1009n5:169": "Devānaṁ pitunañca so;",
  "anya-e1009n5:170": "Karaṁ purisa-kiccāni,",
  "anya-e1009n5:171": "Na ca pacchānutappati.",
  "anya-e1009n5:172": "38.",
  "anya-e1009n5:173": "Hiyyoti hiyyati poso,",
  "anya-e1009n5:174": "Karissāmi pareti yo;",
  "anya-e1009n5:175": "Ajja kattabba kammaṁ sve,",
  "anya-e1009n5:176": "So tato parihāyati.",
  "anya-e1009n5:177": "39.",
  "anya-e1009n5:178": "So a-ppamatto a-kuddho,",
  "anya-e1009n5:179": "Tāta kiccāni kāraya;",
  "anya-e1009n5:180": "Vāyāmassu sa-kiccesu,",
  "anya-e1009n5:181": "Nālaso vindate sukhaṁ.",
  "anya-e1009n5:182": "40.",
  "anya-e1009n5:183": "Hīna-jaccopi ce hoti,",
  "anya-e1009n5:184": "Uṭṭhātā dhitimā naro;",
  "anya-e1009n5:185": "Ācāra-sīla-sampanno,",
  "anya-e1009n5:186": "Nise aggīva bhāsati.",
  "anya-e1009n5:187": "41.",
  "anya-e1009n5:188": "Yo pubbe karaṇīyāni,",
  "anya-e1009n5:189": "Pacchā so kātumicchati;",
  "anya-e1009n5:190": "Varuṇakaṭṭhabhañjova,",
  "anya-e1009n5:191": "Sa pacchā anutappati.",
  "anya-e1009n5:192": "42.",
  "anya-e1009n5:193": "Uṭṭhānakālamhi anuṭṭhahāno,",
  "anya-e1009n5:194": "Yuvā balī ālasiyaṁ upeto;",
  "anya-e1009n5:195": "A-puṇṇasaṅkappamano kusīto,",
  "anya-e1009n5:196": "Paññāya maggaṁ alaso na vindati.",
  "anya-e1009n5:197": "43.",
  "anya-e1009n5:198": "Dummedho puriso loke,",
  "anya-e1009n5:199": "Kusīto hīna-vīriyo;",
  "anya-e1009n5:200": "Appassuto anācāro,",
  "anya-e1009n5:201": "Parihāyati vuḍḍhiyā.",
  "anya-e1009n5:202": "44.",
  "anya-e1009n5:203": "Appassutāyaṁ puriso,",
  "anya-e1009n5:204": "Balībaddova jīrati;",
  "anya-e1009n5:205": "Maṁsāni tassa vaḍḍhanti,",
  "anya-e1009n5:206": "Paññā tassa na vaḍḍhati.",
  "anya-e1009n5:207": "45.",
  "anya-e1009n5:208": "Yassa manussa-bhūtassa,",
  "anya-e1009n5:209": "Natthi bhogā ca sippakaṁ;",
  "anya-e1009n5:210": "Kiṁphalaṁ tassa mānussaṁ,",
  "anya-e1009n5:211": "Dvipādaṭṭho hi so migo.",
  "anya-e1009n5:212": "46.",
  "anya-e1009n5:213": "Yo ca vassasataṁ jīve,",
  "anya-e1009n5:214": "Kusīto hīna-vīriyo;",
  "anya-e1009n5:215": "Ekāhaṁ jīvitaṁ seyyo,",
  "anya-e1009n5:216": "Vīriyārabbhato daḷaṁ.",
  "anya-e1009n5:217": "47.",
  "anya-e1009n5:218": "Yo ca dhamma-vibhaṅgaññū,",
  "anya-e1009n5:219": "Kāluṭṭhāyī a-tandito;",
  "anya-e1009n5:220": "Anuṭṭhahati kālena,",
  "anya-e1009n5:221": "Phalaṁ tassa samijjhati.",
  "anya-e1009n5:222": "48.",
  "anya-e1009n5:223": "A-caritvā brahmacariyaṁ,",
  "anya-e1009n5:224": "A-laddhā yobbane dhanaṁ;",
  "anya-e1009n5:225": "Jiṇṇakoñcāva jhāyanti,",
  "anya-e1009n5:226": "Khīṇamaccheva pallale.",
  "anya-e1009n5:227": "49.",
  "anya-e1009n5:228": "A-caritvā brahmacariyaṁ,",
  "anya-e1009n5:229": "A-laddhā yobbane dhanaṁ;",
  "anya-e1009n5:230": "Sentti cāpātikhīṇāva,",
  "anya-e1009n5:231": "Purāṇāni anutthunaṁ.",
  "anya-e1009n5:232": "50.",
  "anya-e1009n5:233": "Appakenāpi medhāvī,",
  "anya-e1009n5:234": "Pābhatena vicakkhaṇo;",
  "anya-e1009n5:235": "Samuṭṭhāpeti attānaṁ,",
  "anya-e1009n5:236": "Aṇuṁ aggiṁva sandhamaṁ.",
  "anya-e1009n5:237": "51.",
  "anya-e1009n5:238": "Vāyāmetheva puriso,",
  "anya-e1009n5:239": "Na nibbindeyya paṇḍito;",
  "anya-e1009n5:240": "Vāyāmassa phalaṁ passa,",
  "anya-e1009n5:241": "Bhuttā ambā anītihaṁ.",
  "anya-e1009n5:242": "52.",
  "anya-e1009n5:243": "Anuṭṭhahaṁ a-vāyāmaṁ,",
  "anya-e1009n5:244": "Sukhaṁ yatrādhi gacchati;",
  "anya-e1009n5:245": "Suvira tattha gacchāhi,",
  "anya-e1009n5:246": "Mañca tattheva pāpaya.",
  "anya-e1009n5:247": "53.",
  "anya-e1009n5:248": "Yatthālaso anuṭṭhātā,",
  "anya-e1009n5:249": "Accantaṁ sukhamedhati;",
  "anya-e1009n5:250": "Suvira tattha gacchāhi,",
  "anya-e1009n5:251": "Mañca ttattheva pāpaya.",
  "anya-e1009n5:252": "54.",
  "anya-e1009n5:253": "Adhippāya-phalaṁ loke,",
  "anya-e1009n5:254": "Dhītimanttassa sijjhati;",
  "anya-e1009n5:255": "Vīriyameva kattabbaṁ,",
  "anya-e1009n5:256": "Etaṁ buddhehi vaṇṇitaṁ."
}