{
  "ja56:0.1": "Jātaka ",
  "ja56:0.2": "Ekakanipāta ",
  "ja56:0.3": "Āsīsavagga ",
  "ja56:0.4": "6. Kañcanakkhandhajātaka ",
  "ja56:1.1": "“Yo pahaṭṭhena cittena, ",
  "ja56:1.2": "pahaṭṭhamanaso naro; ",
  "ja56:1.3": "Bhāveti kusalaṁ dhammaṁ, ",
  "ja56:1.4": "yogakkhemassa pattiyā; ",
  "ja56:1.5": "Pāpuṇe anupubbena, ",
  "ja56:1.6": "sabbasaṁyojanakkhayan”ti. ",
  "ja56:2.1": "Kañcanakkhandhajātakaṁ chaṭṭhaṁ. "
}