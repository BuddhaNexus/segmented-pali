{
  "ja155:0.1": "Jātaka ",
  "ja155:0.2": "Dukanipāta ",
  "ja155:0.3": "Daḷhavagga ",
  "ja155:0.4": "5. Bhaggajātaka ",
  "ja155:1.1": "“Jīva vassasataṁ bhagga, ",
  "ja155:1.2": "aparāni ca vīsatiṁ; ",
  "ja155:1.3": "Mā maṁ pisācā khādantu, ",
  "ja155:1.4": "jīva tvaṁ sarado sataṁ”. ",
  "ja155:2.1": "“Tvampi vassasataṁ jīva, ",
  "ja155:2.2": "aparāni ca vīsatiṁ; ",
  "ja155:2.3": "Visaṁ pisācā khādantu, ",
  "ja155:2.4": "jīva tvaṁ sarado satan”ti. ",
  "ja155:3.1": "Bhaggajātakaṁ pañcamaṁ. "
}