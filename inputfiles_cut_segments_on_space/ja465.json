{
  "ja465:0.1": "Jātaka ",
  "ja465:0.2": "Dvādasakanipāta ",
  "ja465:0.3": "Cūḷakuṇālavagga ",
  "ja465:0.4": "2. Bhaddasālajātaka ",
  "ja465:1.1": "“Kā tvaṁ suddhehi vatthehi, ",
  "ja465:1.2": "aghe vehāyasaṁ ṭhitā; ",
  "ja465:1.3": "Kena tyāssūni vattanti, ",
  "ja465:1.4": "kuto taṁ bhayamāgataṁ”. ",
  "ja465:2.1": "“Taveva deva vijite, ",
  "ja465:2.2": "bhaddasāloti maṁ vidū; ",
  "ja465:2.3": "Saṭṭhi vassasahassāni, ",
  "ja465:2.4": "tiṭṭhato pūjitassa me. ",
  "ja465:3.1": "Kārayantā nagarāni, ",
  "ja465:3.2": "agāre ca disampati; ",
  "ja465:3.3": "Vividhe cāpi pāsāde, ",
  "ja465:3.4": "na maṁ te accamaññisuṁ; ",
  "ja465:3.5": "Yatheva maṁ te pūjesuṁ, ",
  "ja465:3.6": "tatheva tvampi pūjaya”. ",
  "ja465:4.1": "“Taṁ ivāhaṁ na passāmi, ",
  "ja465:4.2": "thūlaṁ kāyena te dumaṁ; ",
  "ja465:4.3": "Ārohapariṇāhena, ",
  "ja465:4.4": "abhirūposi jātiyā. ",
  "ja465:5.1": "Pāsādaṁ kārayissāmi, ",
  "ja465:5.2": "ekatthambhaṁ manoramaṁ; ",
  "ja465:5.3": "Tattha taṁ upanessāmi, ",
  "ja465:5.4": "ciraṁ te yakkha jīvitaṁ”. ",
  "ja465:6.1": "“Evaṁ cittaṁ udapādi, ",
  "ja465:6.2": "sarīrena vinābhāvo; ",
  "ja465:6.3": "Puthuso maṁ vikantitvā, ",
  "ja465:6.4": "khaṇḍaso avakantatha. ",
  "ja465:7.1": "Agge ca chetvā majjhe ca, ",
  "ja465:7.2": "pacchā mūlamhi chindatha; ",
  "ja465:7.3": "Evaṁ me chijjamānassa, ",
  "ja465:7.4": "na dukkhaṁ maraṇaṁ siyā”. ",
  "ja465:8.1": "“Hatthapādaṁ yathā chinde, ",
  "ja465:8.2": "Kaṇṇanāsañca jīvato; ",
  "ja465:8.3": "Tato pacchā siro chinde, ",
  "ja465:8.4": "Taṁ dukkhaṁ maraṇaṁ siyā. ",
  "ja465:9.1": "Sukhaṁ nu khaṇḍaso chinnaṁ, ",
  "ja465:9.2": "bhaddasāla vanappati; ",
  "ja465:9.3": "Kiṁhetu kiṁ upādāya, ",
  "ja465:9.4": "khaṇḍaso chinnamicchasi”. ",
  "ja465:10.1": "“Yañca hetumupādāya, ",
  "ja465:10.2": "hetuṁ dhammūpasaṁhitaṁ; ",
  "ja465:10.3": "Khaṇḍaso chinnamicchāmi, ",
  "ja465:10.4": "mahārāja suṇohi me. ",
  "ja465:11.1": "Ñātī me sukhasaṁvaddhā, ",
  "ja465:11.2": "mama passe nivātajā; ",
  "ja465:11.3": "Tepihaṁ upahiṁseyyaṁ, ",
  "ja465:11.4": "paresaṁ asukhocitaṁ”. ",
  "ja465:12.1": "“Ceteyyarūpaṁ cetesi, ",
  "ja465:12.2": "bhaddasāla vanappati; ",
  "ja465:12.3": "Hitakāmosi ñātīnaṁ, ",
  "ja465:12.4": "abhayaṁ samma dammi te”ti. ",
  "ja465:13.1": "Bhaddasālajātakaṁ dutiyaṁ. "
}