{
  "ja271:0.1": "Jātaka ",
  "ja271:0.2": "Tikanipāta ",
  "ja271:0.3": "Udapānavagga ",
  "ja271:0.4": "1. Udapānadūsakajātaka ",
  "ja271:1.1": "“Āraññikassa isino, ",
  "ja271:1.2": "cirarattatapassino; ",
  "ja271:1.3": "Kicchākataṁ udapānaṁ, ",
  "ja271:1.4": "kathaṁ samma avāhayi”. ",
  "ja271:2.1": "“Esa dhammo siṅgālānaṁ, ",
  "ja271:2.2": "yaṁ pitvā ohadāmase; ",
  "ja271:2.3": "Pitupitāmahaṁ dhammo, ",
  "ja271:2.4": "na taṁ ujjhātumarahasi”. ",
  "ja271:3.1": "“Yesaṁ vo ediso dhammo, ",
  "ja271:3.2": "Adhammo pana kīdiso; ",
  "ja271:3.3": "Mā vo dhammaṁ adhammaṁ vā, ",
  "ja271:3.4": "Addasāma kudācanan”ti. ",
  "ja271:4.1": "Udapānadūsakajātakaṁ paṭhamaṁ. "
}