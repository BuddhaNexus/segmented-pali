{
  "ja401:0.1": "Jātaka ",
  "ja401:0.2": "Sattakanipāta ",
  "ja401:0.3": "Kukkuvagga ",
  "ja401:0.4": "6. Paṇṇakajātaka ",
  "ja401:1.1": "“Paṇṇakaṁ tikhiṇadhāraṁ, ",
  "ja401:1.2": "Asiṁ sampannapāyinaṁ; ",
  "ja401:1.3": "Parisāyaṁ puriso gilati, ",
  "ja401:1.4": "Kiṁ dukkarataraṁ tato; ",
  "ja401:1.5": "Yadaññaṁ dukkaraṁ ṭhānaṁ, ",
  "ja401:1.6": "Taṁ me akkhāhi pucchito”. ",
  "ja401:2.1": "“Gileyya puriso lobhā, ",
  "ja401:2.2": "asiṁ sampannapāyinaṁ; ",
  "ja401:2.3": "Yo ca vajjā dadāmīti, ",
  "ja401:2.4": "taṁ dukkarataraṁ tato; ",
  "ja401:2.5": "Sabbaññaṁ sukaraṁ ṭhānaṁ, ",
  "ja401:2.6": "evaṁ jānāhi maddava”. ",
  "ja401:3.1": "“Byākāsi āyuro pañhaṁ, ",
  "ja401:3.2": "atthaṁ dhammassa kovido; ",
  "ja401:3.3": "Pukkusaṁ dāni pucchāmi, ",
  "ja401:3.4": "kiṁ dukkarataraṁ tato; ",
  "ja401:3.5": "Yadaññaṁ dukkaraṁ ṭhānaṁ, ",
  "ja401:3.6": "taṁ me akkhāhi pucchito”. ",
  "ja401:4.1": "“Na vācamupajīvanti, ",
  "ja401:4.2": "aphalaṁ giramudīritaṁ; ",
  "ja401:4.3": "Yo ca datvā avākayirā, ",
  "ja401:4.4": "taṁ dukkarataraṁ tato; ",
  "ja401:4.5": "Sabbaññaṁ sukaraṁ ṭhānaṁ, ",
  "ja401:4.6": "evaṁ jānāhi maddava”. ",
  "ja401:5.1": "“Byākāsi pukkuso pañhaṁ, ",
  "ja401:5.2": "atthaṁ dhammassa kovido; ",
  "ja401:5.3": "Senakaṁ dāni pucchāmi, ",
  "ja401:5.4": "kiṁ dukkarataraṁ tato; ",
  "ja401:5.5": "Yadaññaṁ dukkaraṁ ṭhānaṁ, ",
  "ja401:5.6": "taṁ me akkhāhi pucchito”. ",
  "ja401:6.1": "“Dadeyya puriso dānaṁ, ",
  "ja401:6.2": "appaṁ vā yadi vā bahuṁ; ",
  "ja401:6.3": "Yo ca datvā nānutappe, ",
  "ja401:6.4": "taṁ dukkarataraṁ tato; ",
  "ja401:6.5": "Sabbaññaṁ sukaraṁ ṭhānaṁ, ",
  "ja401:6.6": "evaṁ jānāhi maddava”. ",
  "ja401:7.1": "“Byākāsi āyuro pañhaṁ, ",
  "ja401:7.2": "atho pukkusaporiso; ",
  "ja401:7.3": "Sabbe pañhe atibhoti, ",
  "ja401:7.4": "yathā bhāsati senako”ti. ",
  "ja401:8.1": "Paṇṇakajātakaṁ chaṭṭhaṁ. "
}