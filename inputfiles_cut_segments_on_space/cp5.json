{
  "cp5:0.1": "Cariyāpiṭaka ",
  "cp5:0.2": "Akittivagga ",
  "cp5:0.3": "Dānapāramī 5 ",
  "cp5:0.4": "Mahāgovindacariya ",
  "cp5:1.1": "“Punāparaṁ yadā homi, ",
  "cp5:1.2": "sattarājapurohito; ",
  "cp5:1.3": "Pūjito naradevehi, ",
  "cp5:1.4": "mahāgovindabrāhmaṇo. ",
  "cp5:2.1": "Tadāhaṁ sattarajjesu, ",
  "cp5:2.2": "Yaṁ me āsi upāyanaṁ; ",
  "cp5:2.3": "Tena demi mahādānaṁ, ",
  "cp5:2.4": "Akkhobbhaṁ sāgarūpamaṁ. ",
  "cp5:3.1": "Na me dessaṁ dhanaṁ dhaññaṁ, ",
  "cp5:3.2": "Napi natthi nicayo mayi; ",
  "cp5:3.3": "Sabbaññutaṁ piyaṁ mayhaṁ, ",
  "cp5:3.4": "Tasmā demi varaṁ dhanan”ti. ",
  "cp5:4.1": "Mahāgovindacariyaṁ pañcamaṁ. "
}