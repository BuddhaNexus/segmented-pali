{
  "ja222:0.1": "Jātaka ",
  "ja222:0.2": "Dukanipāta ",
  "ja222:0.3": "Kāsāvavagga ",
  "ja222:0.4": "2. Cūḷanandiyajātaka ",
  "ja222:1.1": "“Idaṁ tadācariyavaco, ",
  "ja222:1.2": "pārāsariyo yadabravi; ",
  "ja222:1.3": "Māsu tvaṁ akari pāpaṁ, ",
  "ja222:1.4": "yaṁ tvaṁ pacchā kataṁ tape. ",
  "ja222:2.1": "Yāni karoti puriso, ",
  "ja222:2.2": "tāni attani passati; ",
  "ja222:2.3": "Kalyāṇakārī kalyāṇaṁ, ",
  "ja222:2.4": "pāpakārī ca pāpakaṁ; ",
  "ja222:2.5": "Yādisaṁ vapate bījaṁ, ",
  "ja222:2.6": "tādisaṁ harate phalan”ti. ",
  "ja222:3.1": "Cūḷanandiyajātakaṁ dutiyaṁ. "
}