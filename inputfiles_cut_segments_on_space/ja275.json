{
  "ja275:0.1": "Jātaka ",
  "ja275:0.2": "Tikanipāta ",
  "ja275:0.3": "Udapānavagga ",
  "ja275:0.4": "5. Rucirajātaka ",
  "ja275:1.1": "“Kāyaṁ balākā rucirā, ",
  "ja275:1.2": "kākanīḷasmimacchati; ",
  "ja275:1.3": "Caṇḍo kāko sakhā mayhaṁ, ",
  "ja275:1.4": "yassa cetaṁ kulāvakaṁ”. ",
  "ja275:2.1": "“Nanu maṁ samma jānāsi, ",
  "ja275:2.2": "dija sāmākabhojana; ",
  "ja275:2.3": "Akatvā vacanaṁ tuyhaṁ, ",
  "ja275:2.4": "passa lūnosmi āgato”. ",
  "ja275:3.1": "“Punapāpajjasī samma, ",
  "ja275:3.2": "sīlañhi tava tādisaṁ; ",
  "ja275:3.3": "Na hi mānusakā bhogā, ",
  "ja275:3.4": "subhuñjā honti pakkhinā”ti. ",
  "ja275:4.1": "Rucirajātakaṁ pañcamaṁ. "
}