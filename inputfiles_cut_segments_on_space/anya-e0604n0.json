{
  "anya-e0604n0:0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e0604n0:1": "Suttavandanā",
  "anya-e0604n0:2": "1.",
  "anya-e0604n0:3": "Dhammattha desanā vedha,",
  "anya-e0604n0:4": "Catugambhīra duddasaṃ;",
  "anya-e0604n0:5": "Dhammaṃ lokassa desentaṃ,",
  "anya-e0604n0:6": "Dhammarājaṃ namāmahaṃ;",
  "anya-e0604n0:7": "2.",
  "anya-e0604n0:8": "Buddho paccekabuddho ca,",
  "anya-e0604n0:9": "Arahā cakkavattīti;",
  "anya-e0604n0:10": "Thūpāthūpārahaṃ vande,",
  "anya-e0604n0:11": "Catuthūpārahesu taṃ;",
  "anya-e0604n0:12": "3.",
  "anya-e0604n0:13": "Petaseyyo kāmabhogī,",
  "anya-e0604n0:14": "Sīhaseyyo tathāgato;",
  "anya-e0604n0:15": "Sayantaṃ catuseyyesu,",
  "anya-e0604n0:16": "Catutthena name jinaṃ;",
  "anya-e0604n0:17": "4.",
  "anya-e0604n0:18": "Aṇḍajā jalābujā ca;",
  "anya-e0604n0:19": "Saṃsedajo papātikā;",
  "anya-e0604n0:20": "Catuyoni pariccheda,",
  "anya-e0604n0:21": "Ñāṇavantaṃ name jinaṃ;",
  "anya-e0604n0:22": "5.",
  "anya-e0604n0:23": "Sutabuddhonubuddho ca,",
  "anya-e0604n0:24": "Pacceko jinabuddhoti;",
  "anya-e0604n0:25": "Catutthaṃ catubuddhesu,",
  "anya-e0604n0:26": "Vandāmi lokanāyakaṃ;",
  "anya-e0604n0:27": "6.",
  "anya-e0604n0:28": "Rāhuggaṃ attabhāgīnaṃ,",
  "anya-e0604n0:29": "Mandhātā kāmabhogīnaṃ;",
  "anya-e0604n0:30": "Māro adhipateyyānaṃ,",
  "anya-e0604n0:31": "Buddho loke sadevake;",
  "anya-e0604n0:32": "Catuagga paññattīnaṃ,",
  "anya-e0604n0:33": "Aggapattaṃ name jinaṃ.",
  "anya-e0604n0:34": "7.",
  "anya-e0604n0:35": "Dānaṃ piyakathā attha,",
  "anya-e0604n0:36": "Cariyā sadisattatā;",
  "anya-e0604n0:37": "Catusaṅgaha vatthūhi,",
  "anya-e0604n0:38": "Asaṅkhiyesu jātisu;",
  "anya-e0604n0:39": "Lokassa saṅgahetāraṃ,",
  "anya-e0604n0:40": "Vandāmi lokanāyakaṃ;",
  "anya-e0604n0:41": "8.",
  "anya-e0604n0:42": "Lobho doso ca mohoti,",
  "anya-e0604n0:43": "Anto paccatthike tayo;",
  "anya-e0604n0:44": "Dujjaye sabbaverīhi,",
  "anya-e0604n0:45": "Sañjitaṃpi sudhiṃ name;",
  "anya-e0604n0:46": "9.",
  "anya-e0604n0:47": "Jāti puñña mahattāni,",
  "anya-e0604n0:48": "Guṇamahattamuttamaṃ;",
  "anya-e0604n0:49": "Timahatthehi sampannaṃ,",
  "anya-e0604n0:50": "Vandāmi lokanāyakaṃ;",
  "anya-e0604n0:51": "10.",
  "anya-e0604n0:52": "Hetu phala sampadāyo,",
  "anya-e0604n0:53": "Sattupakārasampadā;",
  "anya-e0604n0:54": "Tisampadāhi sampannaṃ,",
  "anya-e0604n0:55": "Vandāmi lokanāyakaṃ.",
  "anya-e0604n0:56": "11.",
  "anya-e0604n0:57": "Ñāṇappahānānubhāva,",
  "anya-e0604n0:58": "Surūpakārasampadā;",
  "anya-e0604n0:59": "Catuphala sampadāhi,",
  "anya-e0604n0:60": "Taṃ sampannaṃ name jinaṃ.",
  "anya-e0604n0:61": "12.",
  "anya-e0604n0:62": "Āsayo ca payogoti,",
  "anya-e0604n0:63": "Sattupakāra sampadā;",
  "anya-e0604n0:64": "Duvidhā yassa sampannā,",
  "anya-e0604n0:65": "Sammā maṃ pātu so jino.",
  "anya-e0604n0:66": "13.",
  "anya-e0604n0:67": "Mahosadhe vessantare,",
  "anya-e0604n0:68": "Pacchimabhavike tathā;",
  "anya-e0604n0:69": "Tijātīsu name vācaṃ,",
  "anya-e0604n0:70": "Bhāsentaṃ jāyanakkhaṇe.",
  "anya-e0604n0:71": "14.",
  "anya-e0604n0:72": "Nemimhi sādhine ceva,",
  "anya-e0604n0:73": "Mandhātari ca guttile;",
  "anya-e0604n0:74": "Catūsu manujattena,",
  "anya-e0604n0:75": "Devalokaṃ gataṃ name.",
  "anya-e0604n0:76": "15.",
  "anya-e0604n0:77": "Saṅkhāro rūpavikāro,",
  "anya-e0604n0:78": "Lakkhaṇaṃ vānanissaṭaṃ;",
  "anya-e0604n0:79": "Nibbānaṃ paññatticeti,",
  "anya-e0604n0:80": "Pañca ñeyyantaguṃ name.",
  "anya-e0604n0:81": "16.",
  "anya-e0604n0:82": "Putta dāra pariccāgā,",
  "anya-e0604n0:83": "Rajjaṅgaṃ jīva cāgīti;",
  "anya-e0604n0:84": "Pañca mahāpariccāge,",
  "anya-e0604n0:85": "Karaṃ sudukkare name.",
  "anya-e0604n0:86": "17.",
  "anya-e0604n0:87": "Rūpañca vedanā saññā,",
  "anya-e0604n0:88": "Saṅkhārā viññāṇantime;",
  "anya-e0604n0:89": "Pañcakkhandhe parijānaṃ,",
  "anya-e0604n0:90": "Tipariññāhi taṃ name.",
  "anya-e0604n0:91": "18.",
  "anya-e0604n0:92": "Manussa deva gatiyo,",
  "anya-e0604n0:93": "Peto ca nirayo tathā;",
  "anya-e0604n0:94": "Tiracchānoti pañcetā,",
  "anya-e0604n0:95": "Chindantaṃ gatiyo name;",
  "anya-e0604n0:96": "19.",
  "anya-e0604n0:97": "Devaputto kileso ca,",
  "anya-e0604n0:98": "Abhisaṅkhāra mārako;",
  "anya-e0604n0:99": "Khandho maccūti pañcete,",
  "anya-e0604n0:100": "Māre vijitavaṃ name.",
  "anya-e0604n0:101": "20.",
  "anya-e0604n0:102": "Purebhattaṃ pacchābhattaṃ,",
  "anya-e0604n0:103": "Purimañceva majjhimaṃ;",
  "anya-e0604n0:104": "Pacchimanti sadā pañca,",
  "anya-e0604n0:105": "Buddhakiccaṃ karaṃ name.",
  "anya-e0604n0:106": "21.",
  "anya-e0604n0:107": "Rūpā saddā gandhā rasā,",
  "anya-e0604n0:108": "Phoṭṭhabbā ca manoramā;",
  "anya-e0604n0:109": "Pañca kāmaguṇe hitvā,",
  "anya-e0604n0:110": "Kheḷaṃva niggataṃ name.",
  "anya-e0604n0:111": "22.",
  "anya-e0604n0:112": "Rāgasallaṃ dosasallaṃ,",
  "anya-e0604n0:113": "Mohā māno ca diṭṭhīti;",
  "anya-e0604n0:114": "Vande vajjātivajjantaṃ,",
  "anya-e0604n0:115": "Pañcasallapanūdanaṃ.",
  "anya-e0604n0:116": "23.",
  "anya-e0604n0:117": "Dīgho majjhima saṃyuttā,",
  "anya-e0604n0:118": "Aṅguttaro ca khuddako;",
  "anya-e0604n0:119": "Iti pañca nikāyehi,",
  "anya-e0604n0:120": "Suvinentaṃ pajaṃ name.",
  "anya-e0604n0:121": "24.",
  "anya-e0604n0:122": "Issariyaṃ yaso dhammo,",
  "anya-e0604n0:123": "Kāmo sirī payattīti;",
  "anya-e0604n0:124": "Bhagyehi chahi sampannaṃ,",
  "anya-e0604n0:125": "Vandāmi lokanāyakaṃ.",
  "anya-e0604n0:126": "25.",
  "anya-e0604n0:127": "Piyāpiyaṃ bhūtābhūtaṃ,",
  "anya-e0604n0:128": "Atthānatthaṃ yathārahaṃ;",
  "anya-e0604n0:129": "Missā chasu suvācaṃva,",
  "anya-e0604n0:130": "Bhāsantaṃ atthakaṃ name.",
  "anya-e0604n0:131": "26.",
  "anya-e0604n0:132": "Alaso ca pamādo ca,",
  "anya-e0604n0:133": "Anuṭṭhānaṃ asaṃyamo;",
  "anya-e0604n0:134": "Niddā tantīti chiddehi,",
  "anya-e0604n0:135": "Chahi muttaṃ sadā name.",
  "anya-e0604n0:136": "27.",
  "anya-e0604n0:137": "Mahākaruṇāsamāpatti,",
  "anya-e0604n0:138": "Yamakappāṭihāriyaṃ;",
  "anya-e0604n0:139": "Āsayānusaye ñāṇaṃ,",
  "anya-e0604n0:140": "Indriyāna paropare;",
  "anya-e0604n0:141": "Sabbaññutā nāvaraṇaṃ,",
  "anya-e0604n0:142": "Chā sādhāraṇikaṃ name.",
  "anya-e0604n0:143": "28.",
  "anya-e0604n0:144": "Kāmacchando ca byāpādo,",
  "anya-e0604n0:145": "Thinamiddhañca saṃsayo;",
  "anya-e0604n0:146": "Avijjuddhacca kukkuccaṃ,",
  "anya-e0604n0:147": "Chavinīvaraṇaṃ name.",
  "anya-e0604n0:148": "29.",
  "anya-e0604n0:149": "Iddhividhaṃ dibbasotaṃ,",
  "anya-e0604n0:150": "Paracitta vijānanā;",
  "anya-e0604n0:151": "Pubbenivāsānussati,",
  "anya-e0604n0:152": "Dibbacakkhāsavakkhayo;",
  "anya-e0604n0:153": "Chaḷabhiññāhi sampannaṃ,",
  "anya-e0604n0:154": "Vandāmi purisuttamaṃ.",
  "anya-e0604n0:155": "30.",
  "anya-e0604n0:156": "Nīla pītā ca odātā,",
  "anya-e0604n0:157": "Mañjiṭṭhā ca pabhassarā;",
  "anya-e0604n0:158": "Lohitāti cha raṃsīhī,",
  "anya-e0604n0:159": "Vijjotantaṃ sadā name.",
  "anya-e0604n0:160": "31.",
  "anya-e0604n0:161": "Assādādīnavo ceva,",
  "anya-e0604n0:162": "Nissaraṇaphalaṃ tathā;",
  "anya-e0604n0:163": "Upāyāṇattīti chadhā,",
  "anya-e0604n0:164": "Saddhammadesakaṃ name.",
  "anya-e0604n0:165": "32.",
  "anya-e0604n0:166": "Rāgo doso ca moho ca,",
  "anya-e0604n0:167": "Vitakkacariyā tathā;",
  "anya-e0604n0:168": "Saddhā buddhīti bhājentaṃ,",
  "anya-e0604n0:169": "Tā chaḷecariyā name.",
  "anya-e0604n0:170": "33.",
  "anya-e0604n0:171": "Rajataṃ kanakaṃ muttā,",
  "anya-e0604n0:172": "Maṇi veḷuriyāni ca;",
  "anya-e0604n0:173": "Vajirañca pavāḷanti,",
  "anya-e0604n0:174": "Seṭṭhi sattadhano viya.",
  "anya-e0604n0:175": "34.",
  "anya-e0604n0:176": "Saddhāsīlaṃ sutaṃcāgo,",
  "anya-e0604n0:177": "Hirī ottappiyaṃ dhanaṃ,",
  "anya-e0604n0:178": "Paññādhananti sambuddhaṃ,",
  "anya-e0604n0:179": "Vande sattamahādhanaṃ.",
  "anya-e0604n0:180": "35.",
  "anya-e0604n0:181": "Cakkaṃ hatthi asso maṇi,",
  "anya-e0604n0:182": "Gahapatitthiyo tathā;",
  "anya-e0604n0:183": "Supariṇāyakocāti,",
  "anya-e0604n0:184": "Sattaratano cakkavattīva.",
  "anya-e0604n0:185": "36.",
  "anya-e0604n0:186": "Sati dhammavicayo ca,",
  "anya-e0604n0:187": "Tatheva vīriyaṃ pīti;",
  "anya-e0604n0:188": "Passaddhi samādhupekkhā,",
  "anya-e0604n0:189": "Saddhamma cakkavattikaṃ;",
  "anya-e0604n0:190": "Satta bojjhaṅgaratanaṃ,",
  "anya-e0604n0:191": "Namāmi purisuttamaṃ.",
  "anya-e0604n0:192": "37.",
  "anya-e0604n0:193": "Yugandharo īsadharo,",
  "anya-e0604n0:194": "Karavīko sudassano;",
  "anya-e0604n0:195": "Nemindharo vinatako,",
  "anya-e0604n0:196": "Assakaṇṇo mahānage;",
  "anya-e0604n0:197": "Sattete hattharūpaṃva,",
  "anya-e0604n0:198": "Sampassantaṃ jinaṃ name.",
  "anya-e0604n0:199": "38.",
  "anya-e0604n0:200": "Kaṇṇamuṇḍo anotatto,",
  "anya-e0604n0:201": "Kuṇālo rathakārako;",
  "anya-e0604n0:202": "Sīhappapāta chaddantā,",
  "anya-e0604n0:203": "Mandākinī mahāsare;",
  "anya-e0604n0:204": "Sattete sabbathā passaṃ,",
  "anya-e0604n0:205": "Diṭṭheva pātikaṃ name.",
  "anya-e0604n0:206": "39.",
  "anya-e0604n0:207": "Manussattaṃ liṅgasampatti,",
  "anya-e0604n0:208": "Hetu satthāradassanaṃ;",
  "anya-e0604n0:209": "Pabbajjā guṇasampatti,",
  "anya-e0604n0:210": "Adhikāro ca chandatā;",
  "anya-e0604n0:211": "Aṭṭhadhamma samodhānā,",
  "anya-e0604n0:212": "Bhinīhārantaguṃ name.",
  "anya-e0604n0:213": "40.",
  "anya-e0604n0:214": "Visaṭṭhaṃ mañju viññeyyaṃ,",
  "anya-e0604n0:215": "Savanīyāvisāriṇā;",
  "anya-e0604n0:216": "Gambhīro binduninnādo,",
  "anya-e0604n0:217": "Aṭṭhaṅgikasaraṃ name.",
  "anya-e0604n0:218": "41.",
  "anya-e0604n0:219": "Cattāri rūpajhānāni,",
  "anya-e0604n0:220": "Tathevārūpajhānāni;",
  "anya-e0604n0:221": "Tā samāpattiyo aṭṭha,",
  "anya-e0604n0:222": "Sevantaṃ ruciyā name.",
  "anya-e0604n0:223": "42.",
  "anya-e0604n0:224": "Aṇimā laṅghimā kammaṃ,",
  "anya-e0604n0:225": "Mahimā pattimā tathā;",
  "anya-e0604n0:226": "Īsitā vasitā ceva,",
  "anya-e0604n0:227": "Yatthakāmāvasāyitā;",
  "anya-e0604n0:228": "Aṭṭhissariya puṇṇattā,",
  "anya-e0604n0:229": "Issarātissaraṃ name.",
  "anya-e0604n0:230": "43.",
  "anya-e0604n0:231": "Manomayiddhiñāṇañca,",
  "anya-e0604n0:232": "Chaḷabhiññā vipassanā;",
  "anya-e0604n0:233": "Aṭṭhavijjāhi sampannaṃ,",
  "anya-e0604n0:234": "Vande tilokaketukaṃ.",
  "anya-e0604n0:235": "44.",
  "anya-e0604n0:236": "Jāti jarā rujā kālo,",
  "anya-e0604n0:237": "Caturāpāyadukkhāti;",
  "anya-e0604n0:238": "Aṭṭha saṃvegavatthūni,",
  "anya-e0604n0:239": "Bhāsantaṃ vividhaṃ name.",
  "anya-e0604n0:240": "45.",
  "anya-e0604n0:241": "Suttaṅgaṃ veyyākaraṇaṃ,",
  "anya-e0604n0:242": "Geyyaṃ gāthā ca jātakaṃ;",
  "anya-e0604n0:243": "Abbhūtadhamma vedallā,",
  "anya-e0604n0:244": "Udānamiti vuttakaṃ;",
  "anya-e0604n0:245": "Navaṅgaṃ sabbalokassa,",
  "anya-e0604n0:246": "Dadantaṃ sāsanaṃ name.",
  "anya-e0604n0:247": "46.",
  "anya-e0604n0:248": "Yā samāpattiyo aṭṭha,",
  "anya-e0604n0:249": "Nirodho cāti te nava,",
  "anya-e0604n0:250": "Anupubbavihāre taṃ,",
  "anya-e0604n0:251": "Sevantaṃ ruciyā name.",
  "anya-e0604n0:252": "47.",
  "anya-e0604n0:253": "Ekattañceva nānattaṃ,",
  "anya-e0604n0:254": "Kāyasaññīhi yogato,",
  "anya-e0604n0:255": "Cattāro tathāruppā ca,",
  "anya-e0604n0:256": "Asaññīcāti te nava,",
  "anya-e0604n0:257": "Sattavāse vibhajitvā,",
  "anya-e0604n0:258": "Pakāsentaṃ name jinaṃ.",
  "anya-e0604n0:259": "48.",
  "anya-e0604n0:260": "Seyyassa sadiso seyyo,",
  "anya-e0604n0:261": "Hīnoti tividho vidho,",
  "anya-e0604n0:262": "Tathā sadisahīnānaṃ,",
  "anya-e0604n0:263": "Navavidhe nudaṃ name.",
  "anya-e0604n0:264": "49.",
  "anya-e0604n0:265": "Anatthaṃ me carati ca,",
  "anya-e0604n0:266": "Tathā cari carissati,",
  "anya-e0604n0:267": "Mittassa arinotvatthaṃ,",
  "anya-e0604n0:268": "Navāghāte jahaṃ name.",
  "anya-e0604n0:269": "50.",
  "anya-e0604n0:270": "Dānaṃ sīlañca nikkhamaṃ,",
  "anya-e0604n0:271": "Paññā vīriyapañcamaṃ,",
  "anya-e0604n0:272": "Khantī saccamadhiṭṭhānaṃ,",
  "anya-e0604n0:273": "Mettupekkhāti tādasa,",
  "anya-e0604n0:274": "Pūretvā pāramī vande,",
  "anya-e0604n0:275": "Sammāsambodhimajjhavaṃ;",
  "anya-e0604n0:276": "51.",
  "anya-e0604n0:277": "Kāḷāvakañca gaṅgeyyaṃ,",
  "anya-e0604n0:278": "Paṇḍaraṃ tamba piṅgalaṃ,",
  "anya-e0604n0:279": "Gandha maṅgala hemañca,",
  "anya-e0604n0:280": "Uposatha chaddantime.",
  "anya-e0604n0:281": "52.",
  "anya-e0604n0:282": "Dhāreti dasaposānaṃ,",
  "anya-e0604n0:283": "Nāgo kāḷāvako balaṃ;",
  "anya-e0604n0:284": "Sahassakoṭiposānaṃ;",
  "anya-e0604n0:285": "Chaddantovāranuttamo.",
  "anya-e0604n0:286": "53.",
  "anya-e0604n0:287": "Dasa sahassakoṭīnaṃ,",
  "anya-e0604n0:288": "Posānaṃ baladhāraṇaṃ;",
  "anya-e0604n0:289": "Chaddantānaṃ dasannañca,",
  "anya-e0604n0:290": "Dasakāyabalaṃ name.",
  "anya-e0604n0:291": "54.",
  "anya-e0604n0:292": "Ṭhānāṭhāne vipāke ca,",
  "anya-e0604n0:293": "Magge sabbatthagāminaṃ;",
  "anya-e0604n0:294": "Nānādhātūsu lokesu,",
  "anya-e0604n0:295": "Adhimuttimhi pāṇinaṃ;",
  "anya-e0604n0:296": "55.",
  "anya-e0604n0:297": "Paropariyatte ñāṇaṃ,",
  "anya-e0604n0:298": "Indriyānañca pāṇinaṃ;",
  "anya-e0604n0:299": "Jhānādīsu ñāṇaṃ pubbe,",
  "anya-e0604n0:300": "Nivāse dibbacakkhu ca;",
  "anya-e0604n0:301": "Āsavakkhayañāṇanti,",
  "anya-e0604n0:302": "Dasabalañāṇaṃ name.",
  "anya-e0604n0:303": "56.",
  "anya-e0604n0:304": "Mūlagandho sāro pheggu,",
  "anya-e0604n0:305": "Taco papaṭiko raso;",
  "anya-e0604n0:306": "Pattaṃ pupphaṃ phalañceva,",
  "anya-e0604n0:307": "Gandhagandhoti saṃvaraṃ;",
  "anya-e0604n0:308": "Sugandhaṃ dasagandhehi,",
  "anya-e0604n0:309": "Vilimpantaṃ sadā name.",
  "anya-e0604n0:310": "57.",
  "anya-e0604n0:311": "Samādhi ñāṇavipphārā,",
  "anya-e0604n0:312": "Adhiṭṭhānaṃ vikubbanā;",
  "anya-e0604n0:313": "Manomayāriyā iddhi,",
  "anya-e0604n0:314": "Tathā kammavipākajā.",
  "anya-e0604n0:315": "58.",
  "anya-e0604n0:316": "Vijjāmayā puññavato;",
  "anya-e0604n0:317": "Payoga paccayiddhiti;",
  "anya-e0604n0:318": "Aggapatta dasiddhīnaṃ,",
  "anya-e0604n0:319": "Pāṭiheraṃ karaṃ name.",
  "anya-e0604n0:320": "59.",
  "anya-e0604n0:321": "Muttā maṇi veḷuriyaṃ,",
  "anya-e0604n0:322": "Saṅkho sallo pavāḷakaṃ;",
  "anya-e0604n0:323": "Suvaṇṇaṃ rajataṃ lohi,",
  "anya-e0604n0:324": "Taṅko masāragallanti;",
  "anya-e0604n0:325": "Dasadhā ratanehiggaṃ,",
  "anya-e0604n0:326": "Bhogiṃ paññāmaṇiṃ name.",
  "anya-e0604n0:327": "60.",
  "anya-e0604n0:328": "Sithilaṃ dhanitaṃ dīghaṃ,",
  "anya-e0604n0:329": "Rassañca garukaṃ lahu;",
  "anya-e0604n0:330": "Niggahitaṃ vimuttañca,",
  "anya-e0604n0:331": "Sambandhañca vavatthitaṃ;",
  "anya-e0604n0:332": "Vivarantaṃ name dhammaṃ,",
  "anya-e0604n0:333": "Dasabyañjanabheditaṃ.",
  "anya-e0604n0:334": "61.",
  "anya-e0604n0:335": "Pasādojo madhuratā,",
  "anya-e0604n0:336": "Samatā sukhumālatā;",
  "anya-e0604n0:337": "Silesodāratā kanti,",
  "anya-e0604n0:338": "Atthabyatti samādhayo;",
  "anya-e0604n0:339": "Dasa saddaguṇopetaṃ,",
  "anya-e0604n0:340": "Dhammaṃ pātu karaṃ name.",
  "anya-e0604n0:341": "62.",
  "anya-e0604n0:342": "Lobho doso moho māno,",
  "anya-e0604n0:343": "Uddhaccaṃ diṭṭhi saṃsayo;",
  "anya-e0604n0:344": "Ahirikamanottappaṃ,",
  "anya-e0604n0:345": "Thinanti sabba dāhake;",
  "anya-e0604n0:346": "Kilese dasa te saddhiṃ,",
  "anya-e0604n0:347": "Vāsanāya jahaṃ name.",
  "anya-e0604n0:348": "63.",
  "anya-e0604n0:349": "Saterā daṇḍamaṇikā,",
  "anya-e0604n0:350": "Macchavilolagaggarā;",
  "anya-e0604n0:351": "Sukkhāsani kapisisā,",
  "anya-e0604n0:352": "Vicakkā sañña kukkuṭā;",
  "anya-e0604n0:353": "Navāsanīhi phālentaṃ,",
  "anya-e0604n0:354": "Tibbaṃ paññāsaniṃ name.",
  "anya-e0604n0:355": "64.",
  "anya-e0604n0:356": "Rodanā kodhāvudho ca,",
  "anya-e0604n0:357": "Ujjhantissariyaṃ tathā;",
  "anya-e0604n0:358": "Paṭisaṅkhānaṃ,",
  "anya-e0604n0:359": "Khanti aṭṭhabalesu taṃ;",
  "anya-e0604n0:360": "Pathavīsadisaṃ vande,",
  "anya-e0604n0:361": "Aṭṭhamena vināyakaṃ.",
  "anya-e0604n0:362": "65.",
  "anya-e0604n0:363": "Kāmarogo ca paṭigho,",
  "anya-e0604n0:364": "Māno diṭṭhi ca saṃsayo;",
  "anya-e0604n0:365": "Bhavarāgo avijjāti,",
  "anya-e0604n0:366": "Sattānusayinaṃ name.",
  "anya-e0604n0:367": "66.",
  "anya-e0604n0:368": "Titthiyāpakatimahāsāvakā,",
  "anya-e0604n0:369": "Aggasāvakā paccekabuddhasambuddhā;",
  "anya-e0604n0:370": "Visiṭṭhaṃ sabbathā chasu,",
  "anya-e0604n0:371": "Pubbe nivāsa viññūsu;",
  "anya-e0604n0:372": "Tilokamakuṭaṃ name.",
  "anya-e0604n0:373": "67.",
  "anya-e0604n0:374": "Buddhacakkhu samantā ca,",
  "anya-e0604n0:375": "Ñāṇaṃ yaṃ dibbacakkhu ca;",
  "anya-e0604n0:376": "Dhammoti pañca cakkhūhi,",
  "anya-e0604n0:377": "Dassāviṃ mārajiṃ name.",
  "anya-e0604n0:378": "68.",
  "anya-e0604n0:379": "Khuddakā khaṇikā okkantikā ca,",
  "anya-e0604n0:380": "Pharaṇā tathā ubbegā;",
  "anya-e0604n0:381": "Pañca pītīhi pinentaṃ,",
  "anya-e0604n0:382": "Ruciyā ca namāmahaṃ.",
  "anya-e0604n0:383": "69.",
  "anya-e0604n0:384": "Dukkhaṃ samudayasaccaṃ,",
  "anya-e0604n0:385": "Nirodho maggasaccakaṃ;",
  "anya-e0604n0:386": "Catusaccamabhijānaṃ,",
  "anya-e0604n0:387": "Catukiccehi taṃ name.",
  "anya-e0604n0:388": "70.",
  "anya-e0604n0:389": "Pīḷanā saṅkhatattho ca,",
  "anya-e0604n0:390": "Tapo vipariṇāmanā;",
  "anya-e0604n0:391": "Dukkhasaccaṃ catukkehi,",
  "anya-e0604n0:392": "Vibhattāviṃ muniṃ name.",
  "anya-e0604n0:393": "71.",
  "anya-e0604n0:394": "Āyūhanaṃ nidānañca,",
  "anya-e0604n0:395": "Saṃyogo palibodhanaṃ;",
  "anya-e0604n0:396": "Catukkehi samudayaṃ,",
  "anya-e0604n0:397": "Vibhattāviṃ muniṃ name.",
  "anya-e0604n0:398": "72.",
  "anya-e0604n0:399": "Nissaraṇañca viveko,",
  "anya-e0604n0:400": "Asaṅkhatāmataṃ tathā;",
  "anya-e0604n0:401": "Nirodhañca catukkehi,",
  "anya-e0604n0:402": "Vibhattāviṃ muniṃ name.",
  "anya-e0604n0:403": "73.",
  "anya-e0604n0:404": "Niyyāniko ca hetvattho,",
  "anya-e0604n0:405": "Dassanādhipateyyakaṃ;",
  "anya-e0604n0:406": "Maggasaccaṃ catukkehi,",
  "anya-e0604n0:407": "Vibhattāviṃ muniṃ name.",
  "anya-e0604n0:408": "74.",
  "anya-e0604n0:409": "Satthako ca asammoho,",
  "anya-e0604n0:410": "Sappāyo ceva gocaro;",
  "anya-e0604n0:411": "Catusampajaññā vinā,",
  "anya-e0604n0:412": "Bhāviṃ name tathāgataṃ.",
  "anya-e0604n0:413": "75.",
  "anya-e0604n0:414": "Pahinākhila dukkhāhaṃ,",
  "anya-e0604n0:415": "Bhavasāgara pāraguṃ;",
  "anya-e0604n0:416": "Vande sāraguṇopetaṃ,",
  "anya-e0604n0:417": "Tenamhi bhavapārago.",
  "anya-e0604n0:418": "76.",
  "anya-e0604n0:419": "Rūpārūpavilāsagga,",
  "anya-e0604n0:420": "Rūpācinteyya saṃyuttaṃ;",
  "anya-e0604n0:421": "Vande sāraguṇopetaṃ,",
  "anya-e0604n0:422": "Tenamhātularūpavā.",
  "anya-e0604n0:423": "77.",
  "anya-e0604n0:424": "Iddhi iddhi vilāsagga,",
  "anya-e0604n0:425": "Iddhi cinteyya saṃyuttaṃ;",
  "anya-e0604n0:426": "Vande sāraguṇepetaṃ,",
  "anya-e0604n0:427": "Tenamhā tulaiddhimā.",
  "anya-e0604n0:428": "78.",
  "anya-e0604n0:429": "Vācā vācā vilāsagga,",
  "anya-e0604n0:430": "Vācā cinteyya saṃyuttaṃ;",
  "anya-e0604n0:431": "Vande sāraguṇopetaṃ,",
  "anya-e0604n0:432": "Tenamhā tulavācako.",
  "anya-e0604n0:433": "79.",
  "anya-e0604n0:434": "Ñāṇa ñāṇa vilāsagga,",
  "anya-e0604n0:435": "Ñāṇācinteyya saṃyuttaṃ;",
  "anya-e0604n0:436": "Vande sāraguṇopetaṃ,",
  "anya-e0604n0:437": "Tenamhā tulañāṇavā.",
  "anya-e0604n0:438": "80.",
  "anya-e0604n0:439": "Sajjhaṃ hemañca ratanaṃ,",
  "anya-e0604n0:440": "Gehaṃ vatthañca bhojanaṃ;",
  "anya-e0604n0:441": "Tadaññepi yathācittaṃ,",
  "anya-e0604n0:442": "Māpeyyāhaṃ name jinaṃ.",
  "anya-e0604n0:443": "81.",
  "anya-e0604n0:444": "Sajjhaṃ hemañca ratanaṃ,",
  "anya-e0604n0:445": "Gehaṃ vatthañca bhojanaṃ;",
  "anya-e0604n0:446": "Tadaññepi yathācittaṃ,",
  "anya-e0604n0:447": "Māpeyyaṃ kammajiddhiyā.",
  "anya-e0604n0:448": "82.",
  "anya-e0604n0:449": "Sattabojjhaṅga ratano,",
  "anya-e0604n0:450": "Saddhādiratano muni;",
  "anya-e0604n0:451": "Satippabhutiratano,",
  "anya-e0604n0:452": "Vande taṃ purisuttamaṃ.",
  "anya-e0604n0:453": "83.",
  "anya-e0604n0:454": "Cakkādisattaratanaṃ,",
  "anya-e0604n0:455": "Muttādiratanaṃ subhaṃ;",
  "anya-e0604n0:456": "Vatthappabhutiratanaṃ,",
  "anya-e0604n0:457": "Chandakkhaṇe labhāmahaṃ.",
  "anya-e0604n0:458": "84.",
  "anya-e0604n0:459": "Sampuṇṇacittasaṅkappo,",
  "anya-e0604n0:460": "Yathākaṅkhitamāpako,",
  "anya-e0604n0:461": "Kenacinabhibhūto yo,",
  "anya-e0604n0:462": "Sabbābhibhū name jinaṃ.",
  "anya-e0604n0:463": "85.",
  "anya-e0604n0:464": "Sampuṇṇacittasaṅkappo,",
  "anya-e0604n0:465": "Yathākaṅkhitamāpako,",
  "anya-e0604n0:466": "Kenacinabhibhūtamhi,",
  "anya-e0604n0:467": "Sabbābhibhū bhave bhave.",
  "anya-e0604n0:468": "86.",
  "anya-e0604n0:469": "Dasa chaddantarājāva,",
  "anya-e0604n0:470": "Dasa kāyabalo muni;",
  "anya-e0604n0:471": "Dasa ñāṇabbalotulyo,",
  "anya-e0604n0:472": "Vande taṃ samaṇuttamaṃ.",
  "anya-e0604n0:473": "87.",
  "anya-e0604n0:474": "Atikāyajavo buddho,",
  "anya-e0604n0:475": "Raññājavanahaṃsato;",
  "anya-e0604n0:476": "Kā kathā ñāṇavegassa,",
  "anya-e0604n0:477": "Vande taṃ samaṇuttamaṃ.",
  "anya-e0604n0:478": "88.",
  "anya-e0604n0:479": "Chaddantanāgarājāva,",
  "anya-e0604n0:480": "Bhave bhave mahabbalo;",
  "anya-e0604n0:481": "Rājājavanahaṃsova,",
  "anya-e0604n0:482": "Paramaggajavo bhave.",
  "anya-e0604n0:483": "89.",
  "anya-e0604n0:484": "Udakā kāsacārī ca,",
  "anya-e0604n0:485": "Mahinimujjako jino;",
  "anya-e0604n0:486": "Māpako ca yathā cittaṃ,",
  "anya-e0604n0:487": "Vande taṃ ñeyyapāraguṃ.",
  "anya-e0604n0:488": "90.",
  "anya-e0604n0:489": "Udakā kāsacārī ca,",
  "anya-e0604n0:490": "Mahinimujjako bhave;",
  "anya-e0604n0:491": "Māpako ca yathācittaṃ,",
  "anya-e0604n0:492": "Bhavaso kammajiddhiyā.",
  "anya-e0604n0:493": "91.",
  "anya-e0604n0:494": "Gāme vane ca sabbattha,",
  "anya-e0604n0:495": "Devehi manussehi ca;",
  "anya-e0604n0:496": "Ābhatānantalābhassa,",
  "anya-e0604n0:497": "Sadā yassa mahesino;",
  "anya-e0604n0:498": "Pattalābhaggataṃ lokaṃ,",
  "anya-e0604n0:499": "Taṃ vande munipuṅgavaṃ.",
  "anya-e0604n0:500": "92.",
  "anya-e0604n0:501": "Gāme vane ca sabbattha,",
  "anya-e0604n0:502": "Devehi manussehi ca;",
  "anya-e0604n0:503": "Ābhatā nantalābho me,",
  "anya-e0604n0:504": "Sadā hotu bhavābhave.",
  "anya-e0604n0:505": "93.",
  "anya-e0604n0:506": "Puññassimassa tejena,",
  "anya-e0604n0:507": "Yathā cittaṃ samijjhatu;",
  "anya-e0604n0:508": "Sabbicchā sabbacintā ca,",
  "anya-e0604n0:509": "Khippaṃ me jātijātiyaṃ.",
  "anya-e0604n0:510": "94.",
  "anya-e0604n0:511": "Natthīti vacanaṃ dukkhaṃ,",
  "anya-e0604n0:512": "Dehīti vacanaṃ tathā;",
  "anya-e0604n0:513": "Tasmā natthīti dehīti,",
  "anya-e0604n0:514": "Mā me hotu bhavābhave.",
  "anya-e0604n0:515": "95.",
  "anya-e0604n0:516": "Sabbaṃ paravasaṃ dukkhaṃ,",
  "anya-e0604n0:517": "Sabbamissariyaṃ sukhaṃ;",
  "anya-e0604n0:518": "Sabbaṃ paravasamatthu,",
  "anya-e0604n0:519": "Sabbamissariyaṃ bhave.",
  "anya-e0604n0:520": "96.",
  "anya-e0604n0:521": "Vitakkento bhajjakāyo,",
  "anya-e0604n0:522": "Sabbāvudha vāraṇo ca;",
  "anya-e0604n0:523": "Chaddanta vāraṇabalo,",
  "anya-e0604n0:524": "Bhaveyyaṃ jātijā tiyaṃ.",
  "anya-e0604n0:525": "97.",
  "anya-e0604n0:526": "Subhalakkhaṇasampanno,",
  "anya-e0604n0:527": "Suvaṇṇavaṇṇavā bhave;",
  "anya-e0604n0:528": "Brahmassaro karavika,",
  "anya-e0604n0:529": "Bhāṇī ca jātijātiyaṃ.",
  "anya-e0604n0:530": "98.",
  "anya-e0604n0:531": "Bhūrimapañño sippānaṃ,",
  "anya-e0604n0:532": "Sabbesaṃ kusalo bhave;",
  "anya-e0604n0:533": "Visajjetuṃ samatthova,",
  "anya-e0604n0:534": "Sabbapucchānaṃ ṭhānaso.",
  "anya-e0604n0:535": "99.",
  "anya-e0604n0:536": "Verādhaṃsīya bhogā ca,",
  "anya-e0604n0:537": "Anantākhīṇa bhogavā;",
  "anya-e0604n0:538": "Anantābhajja pariso,",
  "anya-e0604n0:539": "Bhavaso pāpuṇe sivaṃ.",
  "anya-e0604n0:540": "100.",
  "anya-e0604n0:541": "Mā nasseyyaṃ pasayheyyaṃ,",
  "anya-e0604n0:542": "Ussukkehi pyahaṃ saha;",
  "anya-e0604n0:543": "Pañcaverehi koṭīhi,",
  "anya-e0604n0:544": "Bhaṇḍaṃ vā akātukāmitaṃ.",
  "anya-e0604n0:545": "101.",
  "anya-e0604n0:546": "Meghaṃ vātañca ratanaṃ,",
  "anya-e0604n0:547": "Dhaññaṃ vatthañca bhojanaṃ;",
  "anya-e0604n0:548": "Sabbicchitaṃ tadaññampi,",
  "anya-e0604n0:549": "Māpeyyaṃ kammajiddhiyā.",
  "anya-e0604n0:550": "102.",
  "anya-e0604n0:551": "Puññenetena nibbānaṃ,",
  "anya-e0604n0:552": "Santaṃ pappomi tāvatā;",
  "anya-e0604n0:553": "Bhaveyyaṃ sabbajātīsu,",
  "anya-e0604n0:554": "Catusampattiyā sadā;",
  "anya-e0604n0:555": "Catucakkena sampanno,",
  "anya-e0604n0:556": "Saddhammehi ca sattahi.",
  "anya-e0604n0:557": "103.",
  "anya-e0604n0:558": "Sammādiṭṭhi vasupete,",
  "anya-e0604n0:559": "Kulamhi seṭṭhasammate,",
  "anya-e0604n0:560": "Sabbasakkata saṃsuddhe;",
  "anya-e0604n0:561": "Bhave tihetusandhiko.",
  "anya-e0604n0:562": "104.",
  "anya-e0604n0:563": "Ghāsacchādanaṃ bhogañca,",
  "anya-e0604n0:564": "Neva hatthena kātuna,",
  "anya-e0604n0:565": "Bhuñjeyyamiddhiyātveva,",
  "anya-e0604n0:566": "Māpetvā yāvadatthakaṃ.",
  "anya-e0604n0:567": "105.",
  "anya-e0604n0:568": "Manussānaṃ atikkamma,",
  "anya-e0604n0:569": "Devānaṃ viya bhojanaṃ,",
  "anya-e0604n0:570": "Vatthaṃ bhogo ca me hontu,",
  "anya-e0604n0:571": "Parivārātisundarā.",
  "anya-e0604n0:572": "106.",
  "anya-e0604n0:573": "Dasāsuttaṃpi me bhogaṃ,",
  "anya-e0604n0:574": "Pañcaverā bhave bhave;",
  "anya-e0604n0:575": "Māgaṇheyyuñca nasseyyuṃ,",
  "anya-e0604n0:576": "Asoko bhogahetume.",
  "anya-e0604n0:577": "107.",
  "anya-e0604n0:578": "Sabbaṅgasubha sampanno,",
  "anya-e0604n0:579": "Siṅgīnikkha savaṇṇa vā;",
  "anya-e0604n0:580": "Bāttiṃsa lakkhaṇūpeto,",
  "anya-e0604n0:581": "Atappana rūpa vā bhave.",
  "anya-e0604n0:582": "108.",
  "anya-e0604n0:583": "Kāyo candanagandho ca,",
  "anya-e0604n0:584": "Mukhaṃ uppalagandhikaṃ;",
  "anya-e0604n0:585": "Aṭṭhaṅgiko karavika,",
  "anya-e0604n0:586": "Mañjūghoso ca me hotu.",
  "anya-e0604n0:587": "109.",
  "anya-e0604n0:588": "Tikkha gambhīra pañño ca,",
  "anya-e0604n0:589": "Hāsātijavapaññavā,",
  "anya-e0604n0:590": "Bhūri nibbedha pañño ca;",
  "anya-e0604n0:591": "Sabbapañhavisajjano.",
  "anya-e0604n0:592": "111.",
  "anya-e0604n0:593": "Anto soḷasavassassa,",
  "anya-e0604n0:594": "Tipeṭakadharo bhave;",
  "anya-e0604n0:595": "Sabbakammesu sippesu,",
  "anya-e0604n0:596": "Vijjāṭhānesu pāragū."
}