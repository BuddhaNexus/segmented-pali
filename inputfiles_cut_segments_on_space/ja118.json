{
  "ja118:0.1": "Jātaka ",
  "ja118:0.2": "Ekakanipāta ",
  "ja118:0.3": "Haṁcivagga ",
  "ja118:0.4": "8. Vaṭṭakajātaka ",
  "ja118:1.1": "“Nācintayanto puriso, ",
  "ja118:1.2": "visesamadhigacchati; ",
  "ja118:1.3": "Cintitassa phalaṁ passa, ",
  "ja118:1.4": "muttosmi vadhabandhanā”ti. ",
  "ja118:2.1": "Vaṭṭakajātakaṁ aṭṭhamaṁ. "
}