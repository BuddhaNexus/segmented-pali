{
  "ja334:0.1": "Jātaka ",
  "ja334:0.2": "Catukkanipāta ",
  "ja334:0.3": "Kokilavagga ",
  "ja334:0.4": "4. Rājovādajātaka ",
  "ja334:1.1": "“Gavañce taramānānaṁ, ",
  "ja334:1.2": "jimhaṁ gacchati puṅgavo; ",
  "ja334:1.3": "Sabbā tā jimhaṁ gacchanti, ",
  "ja334:1.4": "nette jimhaṁ gate sati. ",
  "ja334:2.1": "Evameva manussesu, ",
  "ja334:2.2": "yo hoti seṭṭhasammato; ",
  "ja334:2.3": "So ce adhammaṁ carati, ",
  "ja334:2.4": "pageva itarā pajā; ",
  "ja334:2.5": "Sabbaṁ raṭṭhaṁ dukhaṁ seti, ",
  "ja334:2.6": "rājā ce hoti adhammiko. ",
  "ja334:3.1": "Gavañce taramānānaṁ, ",
  "ja334:3.2": "ujuṁ gacchati puṅgavo; ",
  "ja334:3.3": "Sabbā gāvī ujuṁ yanti, ",
  "ja334:3.4": "nette ujuṁ gate sati. ",
  "ja334:4.1": "Evameva manussesu, ",
  "ja334:4.2": "yo hoti seṭṭhasammato; ",
  "ja334:4.3": "So sace dhammaṁ carati, ",
  "ja334:4.4": "pageva itarā pajā; ",
  "ja334:4.5": "Sabbaṁ raṭṭhaṁ sukhaṁ seti, ",
  "ja334:4.6": "rājā ce hoti dhammiko”ti. ",
  "ja334:5.1": "Rājovādajātakaṁ catutthaṁ. "
}