{
  "ja450:0.1": "Jātaka ",
  "ja450:0.2": "Dasakanipāta ",
  "ja450:0.3": "Catudvāravagga ",
  "ja450:0.4": "12. Bilārakosiyajātaka ",
  "ja450:1.1": "“Apacantāpi dicchanti, ",
  "ja450:1.2": "santo laddhāna bhojanaṁ; ",
  "ja450:1.3": "Kimeva tvaṁ pacamāno, ",
  "ja450:1.4": "yaṁ na dajjā na taṁ samaṁ. ",
  "ja450:2.1": "Maccherā ca pamādā ca, ",
  "ja450:2.2": "evaṁ dānaṁ na diyyati; ",
  "ja450:2.3": "Puññaṁ ākaṅkhamānena, ",
  "ja450:2.4": "deyyaṁ hoti vijānatā. ",
  "ja450:3.1": "Yasseva bhīto na dadāti maccharī, ",
  "ja450:3.2": "Tadevādadato bhayaṁ; ",
  "ja450:3.3": "Jighacchā ca pipāsā ca, ",
  "ja450:3.4": "Yassa bhāyati maccharī; ",
  "ja450:3.5": "Tameva bālaṁ phusati, ",
  "ja450:3.6": "Asmiṁ loke paramhi ca. ",
  "ja450:4.1": "Tasmā vineyya maccheraṁ, ",
  "ja450:4.2": "dajjā dānaṁ malābhibhū; ",
  "ja450:4.3": "Puññāni paralokasmiṁ, ",
  "ja450:4.4": "patiṭṭhā honti pāṇinaṁ”. ",
  "ja450:5.1": "“Duddadaṁ dadamānānaṁ, ",
  "ja450:5.2": "dukkaraṁ kamma kubbataṁ; ",
  "ja450:5.3": "Asanto nānukubbanti, ",
  "ja450:5.4": "sataṁ dhammo durannayo. ",
  "ja450:6.1": "Tasmā satañca asataṁ, ",
  "ja450:6.2": "nānā hoti ito gati; ",
  "ja450:6.3": "Asanto nirayaṁ yanti, ",
  "ja450:6.4": "santo saggaparāyaṇā”. ",
  "ja450:7.1": "“Appasmeke pavecchanti, ",
  "ja450:7.2": "bahuneke na dicchare; ",
  "ja450:7.3": "Appasmā dakkhiṇā dinnā, ",
  "ja450:7.4": "sahassena samaṁ mitā”. ",
  "ja450:8.1": "“Dhammaṁ care yopi samuñchakaṁ care, ",
  "ja450:8.2": "Dārañca posaṁ dadamappakasmiṁ; ",
  "ja450:8.3": "Sataṁ sahassānaṁ sahassayāginaṁ, ",
  "ja450:8.4": "Kalampi nāgghanti tathāvidhassa te”. ",
  "ja450:9.1": "“Kenesa yañño vipulo mahagghato, ",
  "ja450:9.2": "Samena dinnassa na agghameti; ",
  "ja450:9.3": "Kathaṁ sataṁ sahassānaṁ sahassayāginaṁ, ",
  "ja450:9.4": "Kalampi nāgghanti tathāvidhassa te”. ",
  "ja450:10.1": "“Dadanti heke visame niviṭṭhā, ",
  "ja450:10.2": "Chetvā vadhitvā atha socayitvā; ",
  "ja450:10.3": "Sā dakkhiṇā assumukhā sadaṇḍā, ",
  "ja450:10.4": "Samena dinnassa na agghameti; ",
  "ja450:10.5": "Evaṁ sataṁ sahassānaṁ sahassayāginaṁ, ",
  "ja450:10.6": "Kalampi nāgghanti tathāvidhassa te”ti. ",
  "ja450:11.1": "Bilārakosiyajātakaṁ dvādasamaṁ. "
}