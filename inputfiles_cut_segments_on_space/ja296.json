{
  "ja296:0.1": "Jātaka ",
  "ja296:0.2": "Tikanipāta ",
  "ja296:0.3": "Kumbhavagga ",
  "ja296:0.4": "6. Samuddajātaka ",
  "ja296:1.1": "“Ko nāyaṁ loṇatoyasmiṁ, ",
  "ja296:1.2": "samantā paridhāvati; ",
  "ja296:1.3": "Macche makare ca vāreti, ",
  "ja296:1.4": "ūmīsu ca vihaññati”. ",
  "ja296:2.1": "“Anantapāyī sakuṇo, ",
  "ja296:2.2": "atittoti disāsuto; ",
  "ja296:2.3": "Samuddaṁ pātumicchāmi, ",
  "ja296:2.4": "sāgaraṁ saritaṁ patiṁ”. ",
  "ja296:3.1": "“So ayaṁ hāyati ceva, ",
  "ja296:3.2": "pūrate ca mahodadhi; ",
  "ja296:3.3": "Nāssa nāyati pītanto, ",
  "ja296:3.4": "apeyyo kira sāgaro”ti. ",
  "ja296:4.1": "Samuddajātakaṁ chaṭṭhaṁ. "
}