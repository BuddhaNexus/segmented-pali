{
  "anya-e1004n2:0": "Mittakathā",
  "anya-e1004n2:1": "113.",
  "anya-e1004n2:2": "Katvāna kusalaṁkammaṁ,",
  "anya-e1004n2:3": "Katvācā kusalaṁ pure;",
  "anya-e1004n2:4": "Sukhitadukkhitā honti,",
  "anya-e1004n2:5": "So bālo yo napassati.",
  "anya-e1004n2:6": "114.",
  "anya-e1004n2:7": "Sayaṁkatena pāpena,",
  "anya-e1004n2:8": "Aniṭṭhaṁ labhate phalaṁ;",
  "anya-e1004n2:9": "Te me so me janentīti,",
  "anya-e1004n2:10": "Punāguṁ kurute jaḷo.",
  "anya-e1004n2:11": "115.",
  "anya-e1004n2:12": "Kālakhepena hāpeti,",
  "anya-e1004n2:13": "Dānasīlādikaṁ jaḷo;",
  "anya-e1004n2:14": "Athirampi thiraṁ maññe,",
  "anya-e1004n2:15": "Attānaṁ sassatīsamaṁ.",
  "anya-e1004n2:16": "116.",
  "anya-e1004n2:17": "Bālova pāpakaṁ katvā,",
  "anya-e1004n2:18": "Nataṁchaṭṭetumussahe;",
  "anya-e1004n2:19": "Kiṁ byagghatādi gacchanto,",
  "anya-e1004n2:20": "Padaṁ makkhetumussahe.",
  "anya-e1004n2:21": "117.",
  "anya-e1004n2:22": "Paranassanato naṭṭho,",
  "anya-e1004n2:23": "Pureva paranāsako;",
  "anya-e1004n2:24": "Sīghaṁvā dassanaṁ yāti,",
  "anya-e1004n2:25": "Tiṇaṁ pāsādajjhāpakaṁ.",
  "anya-e1004n2:26": "118.",
  "anya-e1004n2:27": "Bhojanā methunā niddā,",
  "anya-e1004n2:28": "Gave poseca vijjati;",
  "anya-e1004n2:29": "Vijjā viseso posassa,",
  "anya-e1004n2:30": "Tatohīno gosamo bhave.",
  "anya-e1004n2:31": "119.",
  "anya-e1004n2:32": "Muḷhasisso padesena,",
  "anya-e1004n2:33": "Kunārībharaṇenaca;",
  "anya-e1004n2:34": "Khalasatthūhisaṁyogā,",
  "anya-e1004n2:35": "Paṇḍitopyavasīdati.",
  "anya-e1004n2:36": "120.",
  "anya-e1004n2:37": "Dvecimekaṇṭakā tikkhā,",
  "anya-e1004n2:38": "Sarīrasobhitā kāme;",
  "anya-e1004n2:39": "Nidhano yoca kāmeti,",
  "anya-e1004n2:40": "Yocakuppatyanissāro.",
  "anya-e1004n2:41": "121.",
  "anya-e1004n2:42": "Nidhanocāpi kāmeti,",
  "anya-e1004n2:43": "Dubbalo kalahaṁpiyo;",
  "anya-e1004n2:44": "Mandasattho vivādatthi,",
  "anya-e1004n2:45": "Tividhaṁ muḷhalakkhaṇaṁ.",
  "anya-e1004n2:46": "122.",
  "anya-e1004n2:47": "Appasuto sute appe,",
  "anya-e1004n2:48": "Bahuṁmaññati mānavo;",
  "anya-e1004n2:49": "Sindhudakama passanto,",
  "anya-e1004n2:50": "Kūpetoyaṁva maṇḍuko.",
  "anya-e1004n2:51": "123.",
  "anya-e1004n2:52": "Tadamināpi jānāti,",
  "anya-e1004n2:53": "Sobbhesu padaresuca;",
  "anya-e1004n2:54": "Sunantā yanti kusumbhā,",
  "anya-e1004n2:55": "Tuṇhīyanti mahodadhi.",
  "anya-e1004n2:56": "124.",
  "anya-e1004n2:57": "Yaṁ ūnakaṁ taṁ sunati,",
  "anya-e1004n2:58": "Yaṁ pūraṁ santameva taṁ;",
  "anya-e1004n2:59": "Aḍḍhakumbhūpamo bālo,",
  "anya-e1004n2:60": "Rahato pūrova paṇḍito.",
  "anya-e1004n2:61": "125.",
  "anya-e1004n2:62": "Budhehibhāsamānopi,",
  "anya-e1004n2:63": "Khalo bahutaketavo;",
  "anya-e1004n2:64": "Ghaṁsiyamāno aṅgāro,",
  "anya-e1004n2:65": "Nilamattaṁ nagacchati.",
  "anya-e1004n2:66": "126.",
  "anya-e1004n2:67": "Cārutā paradārāya,",
  "anya-e1004n2:68": "Dhanaṁ lokatappatiyā;",
  "anya-e1004n2:69": "Pasutaṁ sādhunāsāya,",
  "anya-e1004n2:70": "Khale khalatarā guṇā.",
  "anya-e1004n2:71": "127.",
  "anya-e1004n2:72": "Ito hassataraṁ loke,",
  "anya-e1004n2:73": "Kiñci tassa navijjati;",
  "anya-e1004n2:74": "Dujjanotica yaṁāha,",
  "anya-e1004n2:75": "Sajjanaṁ dujjano sayaṁ.",
  "anya-e1004n2:76": "128.",
  "anya-e1004n2:77": "Rogaṇḍacaṅkuro tejo,",
  "anya-e1004n2:78": "Visamassataro ghaṇo;",
  "anya-e1004n2:79": "Avināsiya samanti,",
  "anya-e1004n2:80": "Na khaloca sanissayaṁ.",
  "anya-e1004n2:81": "129.",
  "anya-e1004n2:82": "Navinā paravādena,",
  "anya-e1004n2:83": "Ramante dujjanā khalu;",
  "anya-e1004n2:84": "Na sā sabbarase bhutvā,",
  "anya-e1004n2:85": "Vinā vacce natussati.",
  "anya-e1004n2:86": "130.",
  "anya-e1004n2:87": "Tappate yāti sandhānaṁ,",
  "anya-e1004n2:88": "Davo bhavati vanataṁ;",
  "anya-e1004n2:89": "Muduṁ dujjanacittaṁ kiṁ,",
  "anya-e1004n2:90": "Lohena upamīyate.",
  "anya-e1004n2:91": "131.",
  "anya-e1004n2:92": "Tasmā dujjanasaṁsaggaṁ,",
  "anya-e1004n2:93": "Āsīvisa mivoragaṁ;",
  "anya-e1004n2:94": "Ārakā parivajjeyya,",
  "anya-e1004n2:95": "Bhūtikāmo vicakkhaṇo.",
  "anya-e1004n2:96": "132.",
  "anya-e1004n2:97": "Dujjanena hi saṁsaggo,",
  "anya-e1004n2:98": "Sattutāpi nayujjate;",
  "anya-e1004n2:99": "Tatto dahati aṅgāro,",
  "anya-e1004n2:100": "Santātu kālataṁkaro.",
  "anya-e1004n2:101": "133.",
  "anya-e1004n2:102": "Dujjano vajjanīyova,",
  "anya-e1004n2:103": "Vijjāyālaṅkatopi ce;",
  "anya-e1004n2:104": "Maṇinā laṅkato santo,",
  "anya-e1004n2:105": "Sappo kiṁ nabhayaṅkaro.",
  "anya-e1004n2:106": "134.",
  "anya-e1004n2:107": "Aggino dahato dāyaṁ,",
  "anya-e1004n2:108": "Sakhābhavati māluto;",
  "anya-e1004n2:109": "Sova dīpaṁtu nāseti,",
  "anya-e1004n2:110": "Khale nattheva mittatā.",
  "anya-e1004n2:111": "135.",
  "anya-e1004n2:112": "Nasmase katapāpamhi,",
  "anya-e1004n2:113": "Nasmase alikavādini;",
  "anya-e1004n2:114": "Nasmasettatthapaññamhi,",
  "anya-e1004n2:115": "Atisantepi nasmase.",
  "anya-e1004n2:116": "136.",
  "anya-e1004n2:117": "Māretuṁ kittakā sakkā,",
  "anya-e1004n2:118": "Dujjanā gagaṇūpamā;",
  "anya-e1004n2:119": "Māritā kodhacittetu,",
  "anya-e1004n2:120": "Mārito honti dujjanā.",
  "anya-e1004n2:121": "137.",
  "anya-e1004n2:122": "Bhūmi kaṇṭakasaṁkiṇṇā,",
  "anya-e1004n2:123": "Chādituṁ nevasakyate;",
  "anya-e1004n2:124": "Upāhana mattakena,",
  "anya-e1004n2:125": "Channā bhavati medanī.",
  "anya-e1004n2:126": "138.",
  "anya-e1004n2:127": "Sattā sadāpasevanti,",
  "anya-e1004n2:128": "Sodakaṁ vāpiādikaṁ;",
  "anya-e1004n2:129": "Sabhogaṁ sadhanañcevaṁ,",
  "anya-e1004n2:130": "Tucchāce te jahanti te.",
  "anya-e1004n2:131": "139.",
  "anya-e1004n2:132": "Dhanahīnaṁ caje mitto,",
  "anya-e1004n2:133": "Puttadārā sahodarā;",
  "anya-e1004n2:134": "Atthavantaṁva sevanti,",
  "anya-e1004n2:135": "Attho loke mahāsakhā.",
  "anya-e1004n2:136": "140.",
  "anya-e1004n2:137": "Na rūpinī na paññāṇo,",
  "anya-e1004n2:138": "Nakulīno padhānatā;",
  "anya-e1004n2:139": "Kāle vipattisampatte,",
  "anya-e1004n2:140": "Dhanimāva visesatā.",
  "anya-e1004n2:141": "141.",
  "anya-e1004n2:142": "Kalyāṇamittaṁ kantāraṁ,",
  "anya-e1004n2:143": "Yuddhaṁ sabhāyaṁ bhāsituṁ;",
  "anya-e1004n2:144": "Asatthā gantu micchanti,",
  "anya-e1004n2:145": "Muḷhā tecaturojanā.",
  "anya-e1004n2:146": "142.",
  "anya-e1004n2:147": "Ahitā paṭisedhoca,",
  "anya-e1004n2:148": "Hitesuca payojanaṁ;",
  "anya-e1004n2:149": "Byasanesva pariccāgo,",
  "anya-e1004n2:150": "Saṅkhepā mittalakkhaṇaṁ.",
  "anya-e1004n2:151": "143.",
  "anya-e1004n2:152": "Āture byasanepatte,",
  "anya-e1004n2:153": "Dubbhikkhe sattu viggahe;",
  "anya-e1004n2:154": "Rājadvāre susāneca,",
  "anya-e1004n2:155": "Yo tiṭṭhati sabandhavo.",
  "anya-e1004n2:156": "144.",
  "anya-e1004n2:157": "So bandhu yo hiteyutto,",
  "anya-e1004n2:158": "So pitā yo tu posako;",
  "anya-e1004n2:159": "So ñāti yatra visāso,",
  "anya-e1004n2:160": "Sā bhariyā yatra nibbūti.",
  "anya-e1004n2:161": "145.",
  "anya-e1004n2:162": "Hitesino sumittoca,",
  "anya-e1004n2:163": "Viññūca dullabhā janā;",
  "anya-e1004n2:164": "Yatho sadhaṁca sāduṁca,",
  "anya-e1004n2:165": "Rogahārīca sujano.",
  "anya-e1004n2:166": "146.",
  "anya-e1004n2:167": "Agaruko anālasso,",
  "anya-e1004n2:168": "Asaṭṭhoca saccavā suci;",
  "anya-e1004n2:169": "Aluddho atthakāmoca,",
  "anya-e1004n2:170": "Ediso suhaduttamo.",
  "anya-e1004n2:171": "147.",
  "anya-e1004n2:172": "Yo dhuvāni pariccajja,",
  "anya-e1004n2:173": "Adhūvānyupasevati;",
  "anya-e1004n2:174": "Dhuvāpi tassa nassanti,",
  "anya-e1004n2:175": "Adhuvesu kathāvakā.",
  "anya-e1004n2:176": "148.",
  "anya-e1004n2:177": "Luddhamatthenagaṇheyya,",
  "anya-e1004n2:178": "Thaddha mañjalikammunā;",
  "anya-e1004n2:179": "Chandānuvattiyā muḷhaṁ,",
  "anya-e1004n2:180": "Yathābhūtena paṇḍitaṁ.",
  "anya-e1004n2:181": "149.",
  "anya-e1004n2:182": "Uttamaṁ pāṇipātena,",
  "anya-e1004n2:183": "Sūraṁ bhedena vijaye;",
  "anya-e1004n2:184": "Nicaṁ dabbapadānena,",
  "anya-e1004n2:185": "Vikkamena samaṁ jaye.",
  "anya-e1004n2:186": "150.",
  "anya-e1004n2:187": "Yassayassa hi yobhāvo,",
  "anya-e1004n2:188": "Tenatena hi taṁnaraṁ;",
  "anya-e1004n2:189": "Anupavissa medhāvī,",
  "anya-e1004n2:190": "Khippamattavasaṁ naye.",
  "anya-e1004n2:191": "151.",
  "anya-e1004n2:192": "Yena icchati sambandhaṁ,",
  "anya-e1004n2:193": "Tena tīṇi nakāraye;",
  "anya-e1004n2:194": "Vivāda matthasambandhaṁ,",
  "anya-e1004n2:195": "Parokkhe dāradassanaṁ.",
  "anya-e1004n2:196": "152.",
  "anya-e1004n2:197": "Accābhikkhaṇasaṁsaggā,",
  "anya-e1004n2:198": "Asammosaraṇenaca;",
  "anya-e1004n2:199": "Etena mittā jīranti,",
  "anya-e1004n2:200": "Akāle yācanāyaca.",
  "anya-e1004n2:201": "153.",
  "anya-e1004n2:202": "Tasmā nābhikkhaṇaṁ gacche,",
  "anya-e1004n2:203": "Nacagacche cirāciraṁ;",
  "anya-e1004n2:204": "Kālena yācaṁ yāceyya,",
  "anya-e1004n2:205": "Evaṁ mittā najīraye.",
  "anya-e1004n2:206": "154.",
  "anya-e1004n2:207": "Ete bhiyyo samaṁ yanti,",
  "anya-e1004n2:208": "Sandhi tesaṁ najīrati;",
  "anya-e1004n2:209": "Yo adhippannaṁ sahati,",
  "anya-e1004n2:210": "Yoca jānāti desanaṁ.",
  "anya-e1004n2:211": "155.",
  "anya-e1004n2:212": "Sace santā vivādanti,",
  "anya-e1004n2:213": "Khippaṁ sandhiraye puna;",
  "anya-e1004n2:214": "Bālā pattāva bhijjanti,",
  "anya-e1004n2:215": "Na te samathamajjhaguṁ.",
  "anya-e1004n2:216": "156.",
  "anya-e1004n2:217": "Yenamittena saṁsaggo,",
  "anya-e1004n2:218": "Yogakkhemo vihīyati;",
  "anya-e1004n2:219": "Pubbeva jjhābhavantassa,",
  "anya-e1004n2:220": "Rakkhe akkhiva paṇḍito.",
  "anya-e1004n2:221": "157.",
  "anya-e1004n2:222": "Yenamittena saṁsaggā,",
  "anya-e1004n2:223": "Yogakkhemo pavaḍḍhati;",
  "anya-e1004n2:224": "Kareyyattasamaṁ vutti,",
  "anya-e1004n2:225": "Sabbakiccesu paṇḍito.",
  "anya-e1004n2:226": "158.",
  "anya-e1004n2:227": "Guṇo sabbaññutulyopi,",
  "anya-e1004n2:228": "Sīdate ko anissayo;",
  "anya-e1004n2:229": "Anaggha mapimāṇikkaṁ,",
  "anya-e1004n2:230": "Hemaṁ nissāya sobhate.",
  "anya-e1004n2:231": "159.",
  "anya-e1004n2:232": "Pabbepabbe kamenucchu,",
  "anya-e1004n2:233": "Visesarasivaggato;",
  "anya-e1004n2:234": "Tathā sumittiko sādhu,",
  "anya-e1004n2:235": "Viparitova dujjano.",
  "anya-e1004n2:236": "160.",
  "anya-e1004n2:237": "Teneva muninā vuttaṁ,",
  "anya-e1004n2:238": "Yekecilokiyādhammā;",
  "anya-e1004n2:239": "Tathā nibbānagāmīno,",
  "anya-e1004n2:240": "Santi lokuttarādhammā.",
  "anya-e1004n2:241": "161.",
  "anya-e1004n2:242": "Kalyāṇamitta 1 māgamma,",
  "anya-e1004n2:243": "Sabbe te honti pāṇinaṁ;",
  "anya-e1004n2:244": "Tasmā kalyāṇamittesu,",
  "anya-e1004n2:245": "Kātabbo ādaro sadā.",
  "anya-e1004n2:246": "162.",
  "anya-e1004n2:247": "Yo ve kataññū katavediko dhiro,",
  "anya-e1004n2:248": "Kalyāṇamitto daḷhabhattica hoti;",
  "anya-e1004n2:249": "Dukkhitassa sakkaccaṁ kareti kiccaṁ,",
  "anya-e1004n2:250": "Tathāvidhaṁ sappurisāti vadanti.",
  "anya-e1004n2:251": "163.",
  "anya-e1004n2:252": "Yassahi dhammaṁ puriso vijaññā,",
  "anya-e1004n2:253": "Yecassa kaṅkhaṁ vinayanti santo;",
  "anya-e1004n2:254": "Tañhissa dīpañca parāyanañca,",
  "anya-e1004n2:255": "Na tena mittaṁ jirayetha pañño."
}