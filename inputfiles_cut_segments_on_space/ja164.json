{
  "ja164:0.1": "Jātaka ",
  "ja164:0.2": "Dukanipāta ",
  "ja164:0.3": "Santhavavagga ",
  "ja164:0.4": "4. Gijjhajātaka ",
  "ja164:1.1": "“Yannu gijjho yojanasataṁ, ",
  "ja164:1.2": "kuṇapāni avekkhati; ",
  "ja164:1.3": "Kasmā jālañca pāsañca, ",
  "ja164:1.4": "āsajjāpi na bujjhasi”. ",
  "ja164:2.1": "“Yadā parābhavo hoti, ",
  "ja164:2.2": "poso jīvitasaṅkhaye; ",
  "ja164:2.3": "Atha jālañca pāsañca, ",
  "ja164:2.4": "āsajjāpi na bujjhatī”ti. ",
  "ja164:3.1": "Gijjhajātakaṁ catutthaṁ. "
}