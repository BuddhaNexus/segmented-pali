{
  "ja41:0.1": "Jātaka ",
  "ja41:0.2": "Ekakanipāta ",
  "ja41:0.3": "Atthakāmavagga ",
  "ja41:0.4": "1. Losakajātaka ",
  "ja41:1.1": "“Yo atthakāmassa hitānukampino, ",
  "ja41:1.2": "Ovajjamāno na karoti sāsanaṁ; ",
  "ja41:1.3": "Ajiyā pādamolamba, ",
  "ja41:1.4": "Mittako viya socatī”ti. ",
  "ja41:2.1": "Losakajātakaṁ paṭhamaṁ. "
}