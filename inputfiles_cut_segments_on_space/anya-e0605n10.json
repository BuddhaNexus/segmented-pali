{
  "anya-e0605n10:0": "Pāduddhāravimhayadīpanīgāthā",
  "anya-e0605n10:1": "65.",
  "anya-e0605n10:2": "Yāvañcayaṁ ravi caratyacalena ruddhe,",
  "anya-e0605n10:3": "Yāvañca cakkaratanañca payāti loke;",
  "anya-e0605n10:4": "Tāvissaro nabhacaro jitacāturanto,",
  "anya-e0605n10:5": "Hitvā kathaṁ nu padamuddhari so nirāso.",
  "anya-e0605n10:6": "66.",
  "anya-e0605n10:7": "Dīpe mahā ca caturādhikadvesahasse,",
  "anya-e0605n10:8": "Tatrāpi seṭṭhabhajitaṁ varajambudīpaṁ;",
  "anya-e0605n10:9": "Bhūnābhikaṁ kapilavatthupuraṁ surammaṁ,",
  "anya-e0605n10:10": "Hitvā kathaṁ nu padamuddhari so nirāso.",
  "anya-e0605n10:11": "67.",
  "anya-e0605n10:12": "Ñātīnasīti kulato hi sahassa sākye,",
  "anya-e0605n10:13": "Hatthissadhaññadhanino vijitārisaṇghe;",
  "anya-e0605n10:14": "Gottena gotamabhavaṁ pitarañjanaggaṁ,",
  "anya-e0605n10:15": "Hitvā kathaṁ nu padamuddhari so nirāso.",
  "anya-e0605n10:16": "68.",
  "anya-e0605n10:17": "Rammaṁ surammavasatiṁ ratanujjalantaṁ,",
  "anya-e0605n10:18": "Gimhepi vimhayakaraṁ suramandirābhaṁ;",
  "anya-e0605n10:19": "Ussāpitaddhajapaṭākasitātapattaṁ,",
  "anya-e0605n10:20": "Hitvā kathaṁ nu padamuddhari so nirāso.",
  "anya-e0605n10:21": "69.",
  "anya-e0605n10:22": "Sapokkharā pokkharaṇī catasso,",
  "anya-e0605n10:23": "Supupphitā mandirato samantā;",
  "anya-e0605n10:24": "Kokā nadantūpari kokanāde,",
  "anya-e0605n10:25": "Hitvā kathaṁ nu padamuddhari so nirāso.",
  "anya-e0605n10:26": "70.",
  "anya-e0605n10:27": "Sare saroje ruditāḷipāḷi,",
  "anya-e0605n10:28": "Samantato passati pañjarañjasā;",
  "anya-e0605n10:29": "Disvāravindāni mukhāravindaṁ",
  "anya-e0605n10:30": "Nāthassa lajjā viya saṁkujanti.",
  "anya-e0605n10:31": "71.",
  "anya-e0605n10:32": "Madhurā madhurābhirutā,",
  "anya-e0605n10:33": "Caritā padume padumeḷigaṇā;",
  "anya-e0605n10:34": "Vasatiṁ adhunā madhunā,",
  "anya-e0605n10:35": "Akaruṁ jahitaṁ kimidaṁ patinā.",
  "anya-e0605n10:36": "72.",
  "anya-e0605n10:37": "Tamhā rasaṁ madhukarā bhavanaṁ haritvā,",
  "anya-e0605n10:38": "Ninnādino samadhuraṁ madhuraṁ karonti;",
  "anya-e0605n10:39": "Nādena nādamatiriccupavīṇayanti,",
  "anya-e0605n10:40": "Naccanti tā surapure vaṇitā va tāva.",
  "anya-e0605n10:41": "73.",
  "anya-e0605n10:42": "Sañcoditā pīṇapayodharādharā,",
  "anya-e0605n10:43": "Virājitānaṅgajamekhalākhalā;",
  "anya-e0605n10:44": "Suraṅgaṇā vaṅgajaphassadā sadā,",
  "anya-e0605n10:45": "Ramā ramāpenti varaṅgadāgadā.",
  "anya-e0605n10:46": "74.",
  "anya-e0605n10:47": "Karātirattā ratirattarāmā,",
  "anya-e0605n10:48": "Tāḷenti tāḷāvacare samantā;",
  "anya-e0605n10:49": "Naccuggatānekasahassahatthā,",
  "anya-e0605n10:50": "Sakkopi kiṁ sakyasamoti codayuṁ.",
  "anya-e0605n10:51": "75.",
  "anya-e0605n10:52": "Visālanettā hasulā sumajjhā,",
  "anya-e0605n10:53": "Nimbatthanī vimhayagītasaddā;",
  "anya-e0605n10:54": "Alaṅkatā malladharā suvatthā,",
  "anya-e0605n10:55": "Naccanti tāḷāvacarehi ghuṭṭhā.",
  "anya-e0605n10:56": "76.",
  "anya-e0605n10:57": "Yāsaṁ hi loke upamā natthi,",
  "anya-e0605n10:58": "Tāsaṁ hi phassesu kathāvakāsā;",
  "anya-e0605n10:59": "Taṁ tādisaṁ kāmaratiṁnubhonto,",
  "anya-e0605n10:60": "Hitvā kathaṁ nu padamuddhari so nirāso.",
  "anya-e0605n10:61": "77.",
  "anya-e0605n10:62": "Pādepāde valayaviravāmekhalāvīṇānādā,",
  "anya-e0605n10:63": "Gītaṁgītaṁ patiratikaraṁ gāyatī gāyatī sā;",
  "anya-e0605n10:64": "Hatthehatthe valayacalitā sambhamaṁ sambhamanti,",
  "anya-e0605n10:65": "Disvādisvā iti ratikaraṁ yāti hāhā kimīhā."
}