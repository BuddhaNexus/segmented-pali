{
  "ja315:0.1": "Jātaka ",
  "ja315:0.2": "Catukkanipāta ",
  "ja315:0.3": "Pucimandavagga ",
  "ja315:0.4": "5. Sabbamaṁsalābhajātaka ",
  "ja315:1.1": "“Pharusā vata te vācā, ",
  "ja315:1.2": "maṁsaṁ yācanako asi; ",
  "ja315:1.3": "Kilomasadisī vācā, ",
  "ja315:1.4": "kilomaṁ samma dammi te”. ",
  "ja315:2.1": "“Aṅgametaṁ manussānaṁ, ",
  "ja315:2.2": "bhātā loke pavuccati; ",
  "ja315:2.3": "Aṅgassa sadisī vācā, ",
  "ja315:2.4": "aṅgaṁ samma dadāmi te”. ",
  "ja315:3.1": "“Tātāti putto vadamāno, ",
  "ja315:3.2": "kampeti hadayaṁ pitu; ",
  "ja315:3.3": "Hadayassa sadisī vācā, ",
  "ja315:3.4": "hadayaṁ samma dammi te”. ",
  "ja315:4.1": "“Yassa gāme sakhā natthi, ",
  "ja315:4.2": "yathāraññaṁ tatheva taṁ; ",
  "ja315:4.3": "Sabbassa sadisī vācā, ",
  "ja315:4.4": "sabbaṁ samma dadāmi te”ti. ",
  "ja315:5.1": "Sabbamaṁsalābhajātakaṁ pañcamaṁ. "
}