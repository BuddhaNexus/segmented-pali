{
  "ja384:0.1": "Jātaka ",
  "ja384:0.2": "Chakkanipāta ",
  "ja384:0.3": "Avāriyavagga ",
  "ja384:0.4": "9. Dhammadhajajātaka ",
  "ja384:1.1": "“Dhammaṁ caratha ñātayo, ",
  "ja384:1.2": "Dhammaṁ caratha bhaddaṁ vo; ",
  "ja384:1.3": "Dhammacārī sukhaṁ seti, ",
  "ja384:1.4": "Asmiṁ loke paramhi ca”. ",
  "ja384:2.1": "“Bhaddako vatayaṁ pakkhī, ",
  "ja384:2.2": "dijo paramadhammiko; ",
  "ja384:2.3": "Ekapādena tiṭṭhanto, ",
  "ja384:2.4": "dhammamevānusāsati”. ",
  "ja384:3.1": "“Nāssa sīlaṁ vijānātha, ",
  "ja384:3.2": "anaññāya pasaṁsatha; ",
  "ja384:3.3": "Bhutvā aṇḍañca potañca, ",
  "ja384:3.4": "dhammo dhammoti bhāsati. ",
  "ja384:4.1": "Aññaṁ bhaṇati vācāya, ",
  "ja384:4.2": "aññaṁ kāyena kubbati; ",
  "ja384:4.3": "Vācāya no ca kāyena, ",
  "ja384:4.4": "na taṁ dhammaṁ adhiṭṭhito. ",
  "ja384:5.1": "Vācāya sakhilo manoviduggo, ",
  "ja384:5.2": "Channo kūpasayova kaṇhasappo; ",
  "ja384:5.3": "Dhammadhajo gāmanigamāsu sādhu, ",
  "ja384:5.4": "Dujjāno purisena bālisena. ",
  "ja384:6.1": "Imaṁ tuṇḍehi pakkhehi, ",
  "ja384:6.2": "pādā cimaṁ viheṭhatha; ",
  "ja384:6.3": "Chavañhimaṁ vināsetha, ",
  "ja384:6.4": "nāyaṁ saṁvāsanāraho”ti. ",
  "ja384:7.1": "Dhammadhajajātakaṁ navamaṁ. "
}