{
  "ja176:0.1": "Jātaka ",
  "ja176:0.2": "Dukanipāta ",
  "ja176:0.3": "Kalyāṇavagga ",
  "ja176:0.4": "6. Kaḷāyamuṭṭhijātaka ",
  "ja176:1.1": "“Bālo vatāyaṁ dumasākhagocaro, ",
  "ja176:1.2": "Paññā janinda nayimassa vijjati; ",
  "ja176:1.3": "Kaḷāyamuṭṭhiṁ avakiriya kevalaṁ, ",
  "ja176:1.4": "Ekaṁ kaḷāyaṁ patitaṁ gavesati”. ",
  "ja176:2.1": "“Evameva mayaṁ rāja, ",
  "ja176:2.2": "ye caññe atilobhino; ",
  "ja176:2.3": "Appena bahuṁ jiyyāma, ",
  "ja176:2.4": "kaḷāyeneva vānaro”ti. ",
  "ja176:3.1": "Kaḷāyamuṭṭhijātakaṁ chaṭṭhaṁ. "
}