{
  "ja207:0.1": "Jātaka ",
  "ja207:0.2": "Dukanipāta ",
  "ja207:0.3": "Nataṁdaḷhavagga ",
  "ja207:0.4": "7. Assakajātaka ",
  "ja207:1.1": "“Ayamassakarājena, ",
  "ja207:1.2": "deso vicarito mayā; ",
  "ja207:1.3": "Anukāmayakāmena, ",
  "ja207:1.4": "piyena patinā saha. ",
  "ja207:2.1": "Navena sukhadukkhena, ",
  "ja207:2.2": "porāṇaṁ apidhīyati; ",
  "ja207:2.3": "Tasmā assakaraññāva, ",
  "ja207:2.4": "kīṭo piyataro mamā”ti. ",
  "ja207:3.1": "Assakajātakaṁ sattamaṁ. "
}