{
  "anya-e0605n17:0": "Ekaṭhānikādiyamakagāthā",
  "anya-e0605n17:1": "101.",
  "anya-e0605n17:2": " Ākaṅkhakkhākaṅkhaṅga kaṅkhāgaṅgākhāgahaka,",
  "anya-e0605n17:3": "Kaṅkhāgāhakakaṅkhāgha hā hā kaṅkhā kahaṁ kahaṁ.",
  "anya-e0605n17:4": "102.",
  "anya-e0605n17:5": " Appagabbho apagabbho amoho mā pamohako,",
  "anya-e0605n17:6": "Maggamukhaṁ mokhamāha māhā mohamūhakkhamaṁ.",
  "anya-e0605n17:7": "103.",
  "anya-e0605n17:8": " Pāpāpāpabhavaṁ passaṁ pāpāpabhavuggato,",
  "anya-e0605n17:9": "Pāpāpāpabhavāsaṅgā pāpāpāpabhavāgato.",
  "anya-e0605n17:10": "104.",
  "anya-e0605n17:11": " Kusalākusalaṁ passaṁ kusalākusalaṁ caji,",
  "anya-e0605n17:12": "Kusalākusalāsaṅga kusalākusalā cuto."
}