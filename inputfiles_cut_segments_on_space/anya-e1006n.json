{
  "anya-e1006n:6": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1006n:7": "Suttantanīti",
  "anya-e1006n:8": "1.",
  "anya-e1006n:9": "Parā bhavantaṃ purisaṃ,",
  "anya-e1006n:10": "Mayaṃ pucchāma gotamaṃ;",
  "anya-e1006n:11": "Bhavantaṃ puṭṭhu māgamma,",
  "anya-e1006n:12": "Kiṃ parābhavato mukhaṃ.",
  "anya-e1006n:13": "2.",
  "anya-e1006n:14": "Suvijāno bhavaṃhoti,",
  "anya-e1006n:15": "Duvijāno parābhavo;",
  "anya-e1006n:16": "Dhammakāmo bhavaṃ hoti,",
  "anya-e1006n:17": "Dhammadessī parābhavo.",
  "anya-e1006n:18": "3.",
  "anya-e1006n:19": "Itihe taṃ vijānāma,",
  "anya-e1006n:20": "Pathamo so parābhavo;",
  "anya-e1006n:21": "Dutīyaṃ bhagavā brūhi,",
  "anya-e1006n:22": "Kiṃ parābhavato mukhaṃ.",
  "anya-e1006n:23": "4.",
  "anya-e1006n:24": "Asantassa piyo hoti,",
  "anya-e1006n:25": "Sante na kurute piyaṃ;",
  "anya-e1006n:26": "Asataṃ dhammaṃ roceti,",
  "anya-e1006n:27": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:28": "Kammāparādhasattānaṃ,",
  "anya-e1006n:29": "Vināse paccupaṭṭhite;",
  "anya-e1006n:30": "Anayo nayarūpena,",
  "anya-e1006n:31": "Buddhimākamya tiṭṭhati.",
  "anya-e1006n:32": "5.",
  "anya-e1006n:33": "Niddāsīlī sabhāsīlī,",
  "anya-e1006n:34": "Anuṭṭhātā ca yo naro;",
  "anya-e1006n:35": "Alaso kodhapaññāṇo,",
  "anya-e1006n:36": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:37": "6.",
  "anya-e1006n:38": "Yo mātaraṃ pitaraṃ vā,",
  "anya-e1006n:39": "Jiṇṇakaṃ gatayobbanaṃ;",
  "anya-e1006n:40": "Pahusanto na bharati,",
  "anya-e1006n:41": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:42": "7.",
  "anya-e1006n:43": "Yo brāhmaṇaṃ samaṇaṃ vā,",
  "anya-e1006n:44": "Aññaṃ vāpi vaṇibbakaṃ;",
  "anya-e1006n:45": "Musāvādena vañceti,",
  "anya-e1006n:46": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:47": "8.",
  "anya-e1006n:48": "Pahutavitto puriso,",
  "anya-e1006n:49": "Sahirañño sabhojano;",
  "anya-e1006n:50": "Eko bhuñjati sādūni,",
  "anya-e1006n:51": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:52": "9.",
  "anya-e1006n:53": "Jātithaddho dhanathaddho,",
  "anya-e1006n:54": "Gottathaddho ca yo naro;",
  "anya-e1006n:55": "Sañātiṃ atimaññeti,",
  "anya-e1006n:56": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:57": "10.",
  "anya-e1006n:58": "Itthidhutto surādhutto,",
  "anya-e1006n:59": "Akkhadhutto ca yo naro;",
  "anya-e1006n:60": "Laddhaṃladdhaṃ vināseti,",
  "anya-e1006n:61": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:62": "Na bāḷhaṃ itthiṃ gaccheyya,",
  "anya-e1006n:63": "Sampassaṃ tejasaṅkhayaṃ;",
  "anya-e1006n:64": "Kāsaṃ sāsaṃ daraṃ bālyaṃ,",
  "anya-e1006n:65": "Khīṇamedo nigacchati.",
  "anya-e1006n:66": "(Ka)",
  "anya-e1006n:67": "Māyācetā marīcī ca,",
  "anya-e1006n:68": "Soko rogo upaddavo;",
  "anya-e1006n:69": "Kharā ca bandhanācetā,",
  "anya-e1006n:70": "Maccupāso guhāsayo.",
  "anya-e1006n:71": "(Kha)",
  "anya-e1006n:72": "Balavanto dubbalā honti,",
  "anya-e1006n:73": "Thāmavantopi hāyare;",
  "anya-e1006n:74": "Cakkhumā andhakā honti,",
  "anya-e1006n:75": "Mātugāmavasaṃgatā.",
  "anya-e1006n:76": "(Ga)",
  "anya-e1006n:77": "Guṇavanto nigguṇā honti,",
  "anya-e1006n:78": "Paññavantopi hāyare;",
  "anya-e1006n:79": "Pamuttā bandhanā senti,",
  "anya-e1006n:80": "Mātugāmavasaṃgatā.",
  "anya-e1006n:81": "(Gha)",
  "anya-e1006n:82": "Yasaṃ kittiṃ dhitiṃ sūraṃ;",
  "anya-e1006n:83": "Bāhussaccaṃ pajānanaṃ;",
  "anya-e1006n:84": "Hāpayanti pamattassa;",
  "anya-e1006n:85": "Kaṭṭhapuñcaṃva pāvako.",
  "anya-e1006n:86": "11.",
  "anya-e1006n:87": "Sehi dārehi santuṭṭho,",
  "anya-e1006n:88": "Vesiyāsu padussati;",
  "anya-e1006n:89": "Dussati paradāresu,",
  "anya-e1006n:90": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:91": "(Ka)",
  "anya-e1006n:92": "Mayañca bhariyaṃ nātikkamāma,",
  "anya-e1006n:93": "Amheca bhariyā nātikkamanti;",
  "anya-e1006n:94": "Aññatra tāhi brahmacariyaṃ carāma,",
  "anya-e1006n:95": "Tasmā hi amhaṃ daharā na miyyare.",
  "anya-e1006n:96": "(Kha)",
  "anya-e1006n:97": "Etāsu jāyare suttamāsu,",
  "anya-e1006n:98": "Medhāvino honti pahutapaññā;",
  "anya-e1006n:99": "Bahussutā theraguṇā ca honti,",
  "anya-e1006n:100": "Tasmā hi amhaṃ daharā na miyyare.",
  "anya-e1006n:101": "12.",
  "anya-e1006n:102": "Atītayobbano poso,",
  "anya-e1006n:103": "Āneti timbarutthaniṃ;",
  "anya-e1006n:104": "Tassa issā na supati,",
  "anya-e1006n:105": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:106": "(Ka)",
  "anya-e1006n:107": "Na dukkhaṃ ahinā daṭṭhaṃ,",
  "anya-e1006n:108": "Na dukkhaṃ sattiyā hataṃ;",
  "anya-e1006n:109": "Tañca dukkhañca tibbañca,",
  "anya-e1006n:110": "Yaṃ passe jiṇṇakaṃ patiṃ.",
  "anya-e1006n:111": "(Kha)",
  "anya-e1006n:112": "Natthi khiṭṭā natthi rati,",
  "anya-e1006n:113": "Jiṇṇena patinā saha;",
  "anya-e1006n:114": "Natthi allāpasallāpo,",
  "anya-e1006n:115": "Jagghituṃpi na sobhati.",
  "anya-e1006n:116": "(Ga)",
  "anya-e1006n:117": "Yadā ca daharo daharī,",
  "anya-e1006n:118": "Mantayiṃsu rahogatā;",
  "anya-e1006n:119": "Sabbe sokā vinassanti,",
  "anya-e1006n:120": "Yekeci hadayassitā.",
  "anya-e1006n:121": "13.",
  "anya-e1006n:122": "Itthiṃ soṇḍiṃ vikiraṇiṃ,",
  "anya-e1006n:123": "Purisaṃ vāpi tādisaṃ;",
  "anya-e1006n:124": "Issariyasmiṃ thapeti,",
  "anya-e1006n:125": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:126": "14.",
  "anya-e1006n:127": "Appabhogo mahātaṇho,",
  "anya-e1006n:128": "Khattiye jāyate kule;",
  "anya-e1006n:129": "So ca rajjaṃ patthayati,",
  "anya-e1006n:130": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:131": "“Suvijāno bhavaṃhoti,",
  "anya-e1006n:132": "Duvijāno parābhavo”;",
  "anya-e1006n:133": "Appabhogo mahātaṇho.",
  "anya-e1006n:134": "15.",
  "anya-e1006n:135": "Ete parābhave loke,",
  "anya-e1006n:136": "Paṇḍito samavekkhiya;",
  "anya-e1006n:137": "Ariyo dassanasampanno,",
  "anya-e1006n:138": "Sa lokaṃ bhajate sivaṃ.",
  "anya-e1006n:139": "Vasalasutta",
  "anya-e1006n:140": "1.",
  "anya-e1006n:141": "Kodhano upanāhīca,",
  "anya-e1006n:142": "Pāpamakkhī ca yo naro;",
  "anya-e1006n:143": "Vipannadiṭṭhī māyāvī,",
  "anya-e1006n:144": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:145": "2.",
  "anya-e1006n:146": "Ekajaṃ vā dvijaṃ vāpi,",
  "anya-e1006n:147": "Yodha pāṇaṃ vihiṃsati;",
  "anya-e1006n:148": "Yassa pāṇe dayā natthi,",
  "anya-e1006n:149": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:150": "3.",
  "anya-e1006n:151": "Yo hanti uparundhati,",
  "anya-e1006n:152": "Gāmāni nigamāni ca;",
  "anya-e1006n:153": "Niggāhako samaññāto,",
  "anya-e1006n:154": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:155": "4.",
  "anya-e1006n:156": "Gāme vā yadi vā raññe,",
  "anya-e1006n:157": "Yaṃ paresaṃ mamāyitaṃ;",
  "anya-e1006n:158": "Theyyā adinnaṃ ādeti,",
  "anya-e1006n:159": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:160": "5.",
  "anya-e1006n:161": "Yo have iṇamādāya,",
  "anya-e1006n:162": "Vuccamāno palāyati;",
  "anya-e1006n:163": "Na hi te iṇamatthīti,",
  "anya-e1006n:164": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:165": "6.",
  "anya-e1006n:166": "Yodha kiñcikkhakamyatā,",
  "anya-e1006n:167": "Pathasmiṃ vajataṃ janaṃ;",
  "anya-e1006n:168": "Hantvā kiñcikkha mādeti;",
  "anya-e1006n:169": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:170": "7.",
  "anya-e1006n:171": "Yo attahetu parahetu,",
  "anya-e1006n:172": "Dhanahetu ca yo naro;",
  "anya-e1006n:173": "Sakkhipuṭṭho musābrūti,",
  "anya-e1006n:174": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:175": "8.",
  "anya-e1006n:176": "Yo ñātīnaṃ sakhīnaṃ vā,",
  "anya-e1006n:177": "Dāresu paṭidissati;",
  "anya-e1006n:178": "Sahasā sampiyena vā,",
  "anya-e1006n:179": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:180": "9.",
  "anya-e1006n:181": "Yo 4 mātaraṃ pitaraṃ vā,",
  "anya-e1006n:182": "Jiṇṇakaṃ gatayobbanaṃ;",
  "anya-e1006n:183": "Pahusanto na bharati,",
  "anya-e1006n:184": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:185": "10.",
  "anya-e1006n:186": "Yo mātaraṃ pitaraṃ vā,",
  "anya-e1006n:187": "Bhātaraṃ bhaginiṃ sassuṃ;",
  "anya-e1006n:188": "Hanti roseti vācāya,",
  "anya-e1006n:189": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:190": "11.",
  "anya-e1006n:191": "Yo atthaṃ pucchito santo,",
  "anya-e1006n:192": "Anattha manusāsati;",
  "anya-e1006n:193": "Paṭicchannena manteti,",
  "anya-e1006n:194": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:195": "12.",
  "anya-e1006n:196": "Yo katvā pāpakaṃ kammaṃ,",
  "anya-e1006n:197": "Māmaṃ jaññāti icchati;",
  "anya-e1006n:198": "Yo paṭicchannakammanto,",
  "anya-e1006n:199": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:200": "13.",
  "anya-e1006n:201": "Yo ve parakulaṃ gantvā,",
  "anya-e1006n:202": "Bhutvāna sucibhojanaṃ;",
  "anya-e1006n:203": "Āgataṃ nappaṭipūjeti,",
  "anya-e1006n:204": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:205": "14.",
  "anya-e1006n:206": "Yo samaṇaṃ vā brāhmaṇaṃ,",
  "anya-e1006n:207": "Aññaṃ vāpi vaṇibbakaṃ;",
  "anya-e1006n:208": "Musāvādena vañceti,",
  "anya-e1006n:209": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:210": "15.",
  "anya-e1006n:211": "Yo samaṇaṃ vā brāhmaṇaṃ,",
  "anya-e1006n:212": "Bhattakāle upaṭṭhitaṃ;",
  "anya-e1006n:213": "Roseti vā na ca deti,",
  "anya-e1006n:214": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:215": "16.",
  "anya-e1006n:216": "Asantaṃ yodha pabrūti,",
  "anya-e1006n:217": "Mohena paliguṇṭhito;",
  "anya-e1006n:218": "Kiñcanaṃ nijigīsāno,",
  "anya-e1006n:219": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:220": "17.",
  "anya-e1006n:221": "Yocattānaṃ samukkaṃse,",
  "anya-e1006n:222": "Pareca mavajānāti;",
  "anya-e1006n:223": "Nihīno sena mānena,",
  "anya-e1006n:224": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:225": "18.",
  "anya-e1006n:226": "Rosako kadarīyo ca,",
  "anya-e1006n:227": "Pāpiccho maccharī saṭho;",
  "anya-e1006n:228": "Ahirīko anottappī,",
  "anya-e1006n:229": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:230": "19.",
  "anya-e1006n:231": "Yo buddhaṃ paribhāsati,",
  "anya-e1006n:232": "Athavā tassa sāvakaṃ;",
  "anya-e1006n:233": "Paribbajaṃ gahaṭṭhaṃ vā,",
  "anya-e1006n:234": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:235_0": "Aṭṭhahi bhikkhave aṅgehi sampannāgatassa upāsakassa patto nikujjitabbo. Bhikkhūnaṃ",
  "anya-e1006n:235_1": "alābhāya parisakkati, bhikkhūnaṃ anatthāya parisakkati, bhikkhūnaṃ anāvāsāya parisakkati,",
  "anya-e1006n:235_2": "bhikkhūnaṃ akkosati paribhāsati, bhikkhūbhikkhūhi bhedeti, buddhassa avaṇṇaṃ bhāsati,",
  "anya-e1006n:235_3": "dhammassa avaṇṇaṃ bhāsati, saṅghassa avaṇṇaṃ bhāsati, anujānāmi bhikkhave imehi aṭṭhahi",
  "anya-e1006n:235_4": "aṅgehi sampannāgatassa upāsakassa pattaṃ nikujjituṃ.",
  "anya-e1006n:236": "20.",
  "anya-e1006n:237": "Yo anarahaṃ santo,",
  "anya-e1006n:238": "Arahāti paṭijānāti;",
  "anya-e1006n:239": "Coro sabrahmake loke,",
  "anya-e1006n:240": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:241": "21.",
  "anya-e1006n:242": "Ete kho vasalā vuttā,",
  "anya-e1006n:243": "Mayā ye te pakāsitā;",
  "anya-e1006n:244": "Na jaccā vasalo hoti,",
  "anya-e1006n:245": "Na jaccā hoti brāhmaṇo.",
  "anya-e1006n:246": "22.",
  "anya-e1006n:247": "Kammunā vasalo hoti,",
  "anya-e1006n:248": "Kammunā hoti brāhmaṇo.",
  "anya-e1006n:249": "23.",
  "anya-e1006n:250": "Tadamināpi jānātha,",
  "anya-e1006n:251": "Yathāhetaṃ nidassanaṃ;",
  "anya-e1006n:252": "Caṇḍālaputto sopāko,",
  "anya-e1006n:253": "Mātaṅgo iti vissuto.",
  "anya-e1006n:254": "24.",
  "anya-e1006n:255": "So yasaṃ paramaṃ patto,",
  "anya-e1006n:256": "Mātaṅgo yaṃ sudullabhaṃ;",
  "anya-e1006n:257": "Āgacchuṃ tassupaṭṭhānaṃ,",
  "anya-e1006n:258": "Khattiyā brāhmaṇā bahū.",
  "anya-e1006n:259": "25.",
  "anya-e1006n:260": "So devayānaṃ abhiruyha,",
  "anya-e1006n:261": "Virajaṃ so mahāpathaṃ;",
  "anya-e1006n:262": "Kāmarāgaṃ virājetvā,",
  "anya-e1006n:263": "Brahmalokūpago ahu.",
  "anya-e1006n:264": "Giriṃ nakhena khaṇasi,",
  "anya-e1006n:265": "Ayo dantebhi khādasi;",
  "anya-e1006n:266": "Jātavedaṃ padahasi,",
  "anya-e1006n:267": "Yo isiṃ paribhāsati.",
  "anya-e1006n:268": "Āvelitaṃ piṭṭhito uttamaṅgaṃ,",
  "anya-e1006n:269": "Bāhuṃ pasāreti akampaṇeyyaṃ;",
  "anya-e1006n:270": "Setāni akkhīni yathā matassa,",
  "anya-e1006n:271": "Ko me imaṃ puttamakāsi evaṃ."
}