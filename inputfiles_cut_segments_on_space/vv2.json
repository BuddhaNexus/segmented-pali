{
  "vv2:0.1": "Vimānavatthu ",
  "vv2:0.2": "Itthivimāna ",
  "vv2:0.3": "Pīṭhavagga ",
  "vv2:0.4": "2. Dutiyapīṭhavimānavatthu ",
  "vv2:1.1": "“Pīṭhaṁ te veḷuriyamayaṁ uḷāraṁ, ",
  "vv2:1.2": "Manojavaṁ gacchati yenakāmaṁ; ",
  "vv2:1.3": "Alaṅkate malyadhare suvatthe, ",
  "vv2:1.4": "Obhāsasi vijjurivabbhakūṭaṁ. ",
  "vv2:2.1": "Kena tetādiso vaṇṇo, ",
  "vv2:2.2": "kena te idha mijjhati; ",
  "vv2:2.3": "Uppajjanti ca te bhogā, ",
  "vv2:2.4": "ye keci manaso piyā. ",
  "vv2:3.1": "Pucchāmi taṁ devi mahānubhāve, ",
  "vv2:3.2": "Manussabhūtā kimakāsi puññaṁ; ",
  "vv2:3.3": "Kenāsi evaṁ jalitānubhāvā, ",
  "vv2:3.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti. ",
  "vv2:4.1": "Sā devatā attamanā, ",
  "vv2:4.2": "moggallānena pucchitā; ",
  "vv2:4.3": "Pañhaṁ puṭṭhā viyākāsi, ",
  "vv2:4.4": "yassa kammassidaṁ phalaṁ. ",
  "vv2:5.1": "“Ahaṁ manussesu manussabhūtā, ",
  "vv2:5.2": "Abbhāgatānāsanakaṁ adāsiṁ; ",
  "vv2:5.3": "Abhivādayiṁ añjalikaṁ akāsiṁ, ",
  "vv2:5.4": "Yathānubhāvañca adāsi dānaṁ. ",
  "vv2:6.1": "Tena metādiso vaṇṇo, ",
  "vv2:6.2": "tena me idha mijjhati; ",
  "vv2:6.3": "Uppajjanti ca me bhogā, ",
  "vv2:6.4": "ye keci manaso piyā. ",
  "vv2:7.1": "Akkhāmi te bhikkhu mahānubhāva, ",
  "vv2:7.2": "Manussabhūtā yamakāsi puññaṁ; ",
  "vv2:7.3": "Tenamhi evaṁ jalitānubhāvā, ",
  "vv2:7.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti. ",
  "vv2:8.1": "Dutiyapīṭhavimānaṁ dutiyaṁ. "
}