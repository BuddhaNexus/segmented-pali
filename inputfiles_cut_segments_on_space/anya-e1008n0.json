{
  "anya-e1008n0:0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1008n0:1": "Cāṇakyanītipāḷi",
  "anya-e1008n0:2": "1.",
  "anya-e1008n0:3": "Nānāsattho ddhataṃ vakkhe,",
  "anya-e1008n0:4": "Rāja nīti samuccayaṃ;",
  "anya-e1008n0:5": "Sabba bīja midaṃ satthaṃ,",
  "anya-e1008n0:6": "Cāṇakyaṃ sārasaṅgahaṃ.",
  "anya-e1008n0:7": "2.",
  "anya-e1008n0:8": "Mūlasuttaṃ pavakkhāmi,",
  "anya-e1008n0:9": "Cāṇakyena yathoditaṃ;",
  "anya-e1008n0:10": "Yassa viññāna mattena,",
  "anya-e1008n0:11": "Mūḷho bhavati paṇḍito.",
  "anya-e1008n0:12": "3.",
  "anya-e1008n0:13": "Viduttaṃ narapaccañca,",
  "anya-e1008n0:14": "Nevatulyaṃ kudācanaṃ;",
  "anya-e1008n0:15": "Sadese pujjate rājā,",
  "anya-e1008n0:16": "Vidū sabbattha pujjate.",
  "anya-e1008n0:17": "4.",
  "anya-e1008n0:18": "Paṇḍite ca guṇā sabbe,",
  "anya-e1008n0:19": "Mūḷhe dosā hi kevalaṃ;",
  "anya-e1008n0:20": "Tasmā mūḷhasahassesu,",
  "anya-e1008n0:21": "Pañño eko visisyate.",
  "anya-e1008n0:22": "5.",
  "anya-e1008n0:23": "Mātariva paradāresu,",
  "anya-e1008n0:24": "Paradabbesu leṭṭuva;",
  "anya-e1008n0:25": "Attaniva sabbabhūtesu,",
  "anya-e1008n0:26": "Yo passati sapaṇḍito.",
  "anya-e1008n0:27": "6.",
  "anya-e1008n0:28": "Kiṃkulena visālena,",
  "anya-e1008n0:29": "Guṇahīno tu yonaro;",
  "anya-e1008n0:30": "Akulinopi satthañño,",
  "anya-e1008n0:31": "Devatāhipi pujjate.",
  "anya-e1008n0:32": "7.",
  "anya-e1008n0:33": "Rūpayobbanasampannā,",
  "anya-e1008n0:34": "Visālakulasambhavā;",
  "anya-e1008n0:35": "Vijjāhīnā nasobhante,",
  "anya-e1008n0:36": "Niggandhā iva kiṃsukā.",
  "anya-e1008n0:37": "8.",
  "anya-e1008n0:38": "Tārānaṃ bhūsaṇaṃ cando,",
  "anya-e1008n0:39": "Nārīnaṃ bhūsaṇaṃ pati;",
  "anya-e1008n0:40": "Puthabyā bhūsaṇaṃ rājā,",
  "anya-e1008n0:41": "Vijjā sabbassa bhūsaṇaṃ.",
  "anya-e1008n0:42": "9.",
  "anya-e1008n0:43": "Mātā sattu pitā verī,",
  "anya-e1008n0:44": "Yena bālo napāṭṭhito;",
  "anya-e1008n0:45": "Na sobhate sabhāmajjhe,",
  "anya-e1008n0:46": "Haṃsamajjhe bakoyathā.",
  "anya-e1008n0:47": "10.",
  "anya-e1008n0:48": "Varameko guṇī putto,",
  "anya-e1008n0:49": "Na ca mūḷhasatehipi;",
  "anya-e1008n0:50": "Eko cando tamo hanti,",
  "anya-e1008n0:51": "Na ca tāragaṇehipi.",
  "anya-e1008n0:52": "11.",
  "anya-e1008n0:53": "Lālaye pañcavassāni,",
  "anya-e1008n0:54": "Dasavassāni tālaye;",
  "anya-e1008n0:55": "Patte tu soḷase vasse,",
  "anya-e1008n0:56": "Puttaṃ mittaṃva ācare.",
  "anya-e1008n0:57": "12.",
  "anya-e1008n0:58": "Lālane bahavo dosā,",
  "anya-e1008n0:59": "Tālane bahavo guṇā;",
  "anya-e1008n0:60": "Tasmā puttañca sissañca,",
  "anya-e1008n0:61": "Tālaye na tu lālaye.",
  "anya-e1008n0:62": "13.",
  "anya-e1008n0:63": "Ekenāpi suvakkhena,",
  "anya-e1008n0:64": "Pupphitena sugandhinā;",
  "anya-e1008n0:65": "Vāsitassa vanaṃ sabbaṃ,",
  "anya-e1008n0:66": "Suputtena kulaṃyathā.",
  "anya-e1008n0:67": "14.",
  "anya-e1008n0:68": "Ekassāpi kuvakkhassa,",
  "anya-e1008n0:69": "Koṭaraṭṭhena agginā;",
  "anya-e1008n0:70": "Dayhate hi vanaṃ sabbaṃ,",
  "anya-e1008n0:71": "Kuputtena kulaṃ yathā.",
  "anya-e1008n0:72": "15.",
  "anya-e1008n0:73": "Dūrato sobhate mūḷho,",
  "anya-e1008n0:74": "Lambamāna paṭāvuto;",
  "anya-e1008n0:75": "Tāvañca sobhate mūḷho,",
  "anya-e1008n0:76": "Yāva kiñci nabhāsate.",
  "anya-e1008n0:77": "16.",
  "anya-e1008n0:78": "Visato amataṃ gāyhaṃ,",
  "anya-e1008n0:79": "Amejjhāapi kañcanaṃ;",
  "anya-e1008n0:80": "Nīcato uttamā vijjā,",
  "anya-e1008n0:81": "Thīratnaṃ dukkulāapi.",
  "anya-e1008n0:82": "17.",
  "anya-e1008n0:83": "Ussave byasaneceva,",
  "anya-e1008n0:84": "Dubbhikkhe sattuviggahe;",
  "anya-e1008n0:85": "Rājadvāre susāneca,",
  "anya-e1008n0:86": "Yo tiṭṭhati sabandhavo.",
  "anya-e1008n0:87": "18.",
  "anya-e1008n0:88": "Parokkhe kiccahantāraṃ,",
  "anya-e1008n0:89": "Paccakkhe piyavādinaṃ;",
  "anya-e1008n0:90": "Vajjaye tādisaṃ mittaṃ,",
  "anya-e1008n0:91": "Visakumbhaṃ payomukhaṃ.",
  "anya-e1008n0:92": "19.",
  "anya-e1008n0:93": "Sakiṃ duṭṭhañca mittaṃ yo,",
  "anya-e1008n0:94": "Puna sandhātu micchati;",
  "anya-e1008n0:95": "Samaccu mupagaṇhāti,",
  "anya-e1008n0:96": "Gabbha massatarī yathā.",
  "anya-e1008n0:97": "20.",
  "anya-e1008n0:98": "Na vissase avissatthaṃ,",
  "anya-e1008n0:99": "Mittañcāpi na vissase;",
  "anya-e1008n0:100": "Kadāci kupitaṃ mittaṃ,",
  "anya-e1008n0:101": "Sabbadosaṃ pakāsaye.",
  "anya-e1008n0:102": "21.",
  "anya-e1008n0:103": "Jāniyā pesane bhacce,",
  "anya-e1008n0:104": "Bandhave byasanāgame;",
  "anya-e1008n0:105": "Mittañcā padikāleca,",
  "anya-e1008n0:106": "Bhariyañca vibhavakkhaye.",
  "anya-e1008n0:107": "22.",
  "anya-e1008n0:108": "Upakāraggahitena,",
  "anya-e1008n0:109": "Sattunāsattumuddhare;",
  "anya-e1008n0:110": "Pādalaggaṃ karaṭṭhena,",
  "anya-e1008n0:111": "Kaṇṭakeneva kaṇṭakaṃ.",
  "anya-e1008n0:112": "23.",
  "anya-e1008n0:113": "Na mittaṃ koci kassaci,",
  "anya-e1008n0:114": "Na koci ripu kassaci;",
  "anya-e1008n0:115": "Kāraṇena hi ñāyati,",
  "anya-e1008n0:116": "Mittāni ca ripū tathā.",
  "anya-e1008n0:117": "24.",
  "anya-e1008n0:118": "Dujjano piyavādī ca,",
  "anya-e1008n0:119": "Netaṃ vissāsakāraṇaṃ;",
  "anya-e1008n0:120": "Madhu tiṭṭhati jivhagge,",
  "anya-e1008n0:121": "Hadaye tu halāhalaṃ.",
  "anya-e1008n0:122": "25.",
  "anya-e1008n0:123": "Dujjano parihantabbo,",
  "anya-e1008n0:124": "Vijjāyā laṅkatopi saṃ;",
  "anya-e1008n0:125": "Maṇinā bhūsito sappo,",
  "anya-e1008n0:126": "Kime so nabhayaṃ karo.",
  "anya-e1008n0:127": "26.",
  "anya-e1008n0:128": "Sappo kūro khalo kūro,",
  "anya-e1008n0:129": "Sappā kūrataro khalo;",
  "anya-e1008n0:130": "Manto sadhivaso sappo,",
  "anya-e1008n0:131": "Khalo kena nivāyyate.",
  "anya-e1008n0:132": "27.",
  "anya-e1008n0:133": "Nakhīnañca nadīnañca,",
  "anya-e1008n0:134": "Siṅgīnaṃ satthapāṇinaṃ;",
  "anya-e1008n0:135": "Vissāso nevakātabbo,",
  "anya-e1008n0:136": "Thīsu rājakulesu ca.",
  "anya-e1008n0:137": "28.",
  "anya-e1008n0:138": "Hatthī hatthasahassena,",
  "anya-e1008n0:139": "Satahatthena vājino;",
  "anya-e1008n0:140": "Siṅgino dasahatthena,",
  "anya-e1008n0:141": "Ṭhānacāgena dujjano.",
  "anya-e1008n0:142": "29.",
  "anya-e1008n0:143": "Āpadatthaṃ dhanaṃ rakkhe,",
  "anya-e1008n0:144": "Dāraṃ rakkhe dhanehipi;",
  "anya-e1008n0:145": "Attānaṃ satataṃ rakkhe,",
  "anya-e1008n0:146": "Dārehipi dhanehipi.",
  "anya-e1008n0:147": "30.",
  "anya-e1008n0:148": "Paradāraṃ paradabbaṃ,",
  "anya-e1008n0:149": "Parivādaṃ parassa ca;",
  "anya-e1008n0:150": "Parihāsaṃ guruṭṭhāne,",
  "anya-e1008n0:151": "Cāpalyañca vivajjaye.",
  "anya-e1008n0:152": "31.",
  "anya-e1008n0:153": "Caje ekaṃ kulassatthe,",
  "anya-e1008n0:154": "Gāmassatthe kulaṃ caje;",
  "anya-e1008n0:155": "Gāma janapadassatthe,",
  "anya-e1008n0:156": "Attatthe pathaviṃ caje.",
  "anya-e1008n0:157": "32.",
  "anya-e1008n0:158": "Calatyekena pādena,",
  "anya-e1008n0:159": "Tiṭṭha tyekena buddhimā;",
  "anya-e1008n0:160": "Nāsamikkhya paraṃ ṭhānaṃ,",
  "anya-e1008n0:161": "Pubbamāyatanaṃ caje.",
  "anya-e1008n0:162": "33.",
  "anya-e1008n0:163": "Luddha matthena gaṇheyya,",
  "anya-e1008n0:164": "Thaddha mañjalī kammunā;",
  "anya-e1008n0:165": "Mūḷhaṃ chando nuvattena,",
  "anya-e1008n0:166": "Tathā tathena paṇḍitaṃ.",
  "anya-e1008n0:167": "34.",
  "anya-e1008n0:168": "Atthanāsaṃ manotāpaṃ,",
  "anya-e1008n0:169": "Gehe duccaritāni ca;",
  "anya-e1008n0:170": "Vañcanañca pamāṇañca,",
  "anya-e1008n0:171": "Matimā na pakāsaye.",
  "anya-e1008n0:172": "35.",
  "anya-e1008n0:173": "Dhanadhaññappayogesu,",
  "anya-e1008n0:174": "Tathā vijjāgamesu ca;",
  "anya-e1008n0:175": "Āhāre byavahāre ca,",
  "anya-e1008n0:176": "Cattalajjo sadā bhave.",
  "anya-e1008n0:177": "36.",
  "anya-e1008n0:178": "Dhanino sotthiyo rājā,",
  "anya-e1008n0:179": "Nadī vejjo tu pañcamo;",
  "anya-e1008n0:180": "Pañca yatra navijjante,",
  "anya-e1008n0:181": "Tatra vāsaṃ nakāraye.",
  "anya-e1008n0:182": "37.",
  "anya-e1008n0:183": "Yasmiṃdese na sammānaṃ,",
  "anya-e1008n0:184": "Na pīti naca bandhavā;",
  "anya-e1008n0:185": "Na ca vijjāgamo koci,",
  "anya-e1008n0:186": "Taṃdesaṃ parivajjaye.",
  "anya-e1008n0:187": "38.",
  "anya-e1008n0:188": "Manasā cintitaṃ kammaṃ,",
  "anya-e1008n0:189": "Vacasā napakāsaye;",
  "anya-e1008n0:190": "Aññalakkhita kāriyassa,",
  "anya-e1008n0:191": "Yato siddhi najāyate.",
  "anya-e1008n0:192": "39.",
  "anya-e1008n0:193": "Kudesañca kuvuttiñca,",
  "anya-e1008n0:194": "Kubhariyaṃ kunadiṃ tathā;",
  "anya-e1008n0:195": "Kudabbañca kubhojjañca,",
  "anya-e1008n0:196": "Vajjaye tu vicakkhaṇo.",
  "anya-e1008n0:197": "40.",
  "anya-e1008n0:198": "Iṇasesoggi seso ca,",
  "anya-e1008n0:199": "Byādhiseso tatheva ca;",
  "anya-e1008n0:200": "Puna ca vaḍḍhate yasmā,",
  "anya-e1008n0:201": "Tasmā sesaṃ nakāraye.",
  "anya-e1008n0:202": "41.",
  "anya-e1008n0:203": "Cintā jaro manussānaṃ,",
  "anya-e1008n0:204": "Vatthānaṃ ātapo jaro;",
  "anya-e1008n0:205": "Asobhagyaṃ jaro thīnaṃ,",
  "anya-e1008n0:206": "Assānaṃ methunaṃ jaro.",
  "anya-e1008n0:207": "42.",
  "anya-e1008n0:208": "Atthi putto vase yassa,",
  "anya-e1008n0:209": "Bhacco bhariyā tatheva ca;",
  "anya-e1008n0:210": "Abhāve pyatisantoso,",
  "anya-e1008n0:211": "Saggaṭṭho so mahītale.",
  "anya-e1008n0:212": "43.",
  "anya-e1008n0:213": "Duṭṭhā bhariyā saṭhaṃ mittaṃ,",
  "anya-e1008n0:214": "Bhacco cuttaradāyako;",
  "anya-e1008n0:215": "Sa sappeca gahe vāso,",
  "anya-e1008n0:216": "Maccureva nasaṃsayo.",
  "anya-e1008n0:217": "44.",
  "anya-e1008n0:218": "Mātā yassa gehe natthi,",
  "anya-e1008n0:219": "Bhariyācā piyavādinī;",
  "anya-e1008n0:220": "Araññaṃ tena gantabbaṃ,",
  "anya-e1008n0:221": "Yathā raññaṃ tathāgahaṃ.",
  "anya-e1008n0:222": "45.",
  "anya-e1008n0:223": "Iṇakattā pitā sattu,",
  "anya-e1008n0:224": "Mātā ca byabhicārinī;",
  "anya-e1008n0:225": "Bhariyā rūpavatī sattu,",
  "anya-e1008n0:226": "Putto sattu apaṇḍito.",
  "anya-e1008n0:227": "46.",
  "anya-e1008n0:228": "Kokilānaṃ saro rūpaṃ,",
  "anya-e1008n0:229": "Nārī rūpaṃ patibbatā;",
  "anya-e1008n0:230": "Vijjā rūpaṃ kurūpānaṃ,",
  "anya-e1008n0:231": "Khamā rūpaṃ tapassinaṃ.",
  "anya-e1008n0:232": "47.",
  "anya-e1008n0:233": "Avijjaṃ jīvanaṃ suññaṃ,",
  "anya-e1008n0:234": "Disā suññā abandhavā;",
  "anya-e1008n0:235": "Puttahīnaṃ gahaṃ suññaṃ,",
  "anya-e1008n0:236": "Sabbasuññā daliddatā.",
  "anya-e1008n0:237": "48.",
  "anya-e1008n0:238": "Adātā vaṃsadosena,",
  "anya-e1008n0:239": "Kammadosā daliddatā;",
  "anya-e1008n0:240": "Ummādā mātudosena,",
  "anya-e1008n0:241": "Pitudosena mūḷhatā.",
  "anya-e1008n0:242": "49.",
  "anya-e1008n0:243": "Guru aggi dvijādīnaṃ,",
  "anya-e1008n0:244": "Vaṇṇānaṃ brāhmaṇo guru;",
  "anya-e1008n0:245": "Pati reko gurutthīnaṃ,",
  "anya-e1008n0:246": "Sabbassābhyāgato guru.",
  "anya-e1008n0:247": "50.",
  "anya-e1008n0:248": "Atidabbe hatā laṅkā,",
  "anya-e1008n0:249": "Atimāne ca koravā;",
  "anya-e1008n0:250": "Atidāne balībaddho,",
  "anya-e1008n0:251": "Sabbamaccanta gahitaṃ.",
  "anya-e1008n0:252": "51.",
  "anya-e1008n0:253": "Vatthahīno tvalaṅkāro,",
  "anya-e1008n0:254": "Ghatahīnañca bhojanaṃ;",
  "anya-e1008n0:255": "Thanahīnā ca yānārī,",
  "anya-e1008n0:256": "Vijjāhīnañca jīvanaṃ.",
  "anya-e1008n0:257": "52.",
  "anya-e1008n0:258": "Bhojjaṃ bhojanasatti ca,",
  "anya-e1008n0:259": "Ratisatti varā thiyo;",
  "anya-e1008n0:260": "Vibhavo dānasatti ca,",
  "anya-e1008n0:261": "Nāppassa tapaso phalaṃ.",
  "anya-e1008n0:262": "53.",
  "anya-e1008n0:263": "Puttappayojanā dārā,",
  "anya-e1008n0:264": "Putto piṇḍappayojano;",
  "anya-e1008n0:265": "Hitappayojanaṃ mittaṃ,",
  "anya-e1008n0:266": "Dhanaṃ sabbappayojanaṃ.",
  "anya-e1008n0:267": "54.",
  "anya-e1008n0:268": "Dullabhaṃ pākatikaṃ vākyaṃ,",
  "anya-e1008n0:269": "Dullabho khemakaro suto;",
  "anya-e1008n0:270": "Dullabhā sadisī jāyā,",
  "anya-e1008n0:271": "Dullabho sajano piyo.",
  "anya-e1008n0:272": "55.",
  "anya-e1008n0:273": "Selesele namāṇikkaṃ,",
  "anya-e1008n0:274": "Muttikaṃ na gajegaje;",
  "anya-e1008n0:275": "Sādhavo na hi sabbatra,",
  "anya-e1008n0:276": "Candanaṃ na vanevane.",
  "anya-e1008n0:277": "56.",
  "anya-e1008n0:278": "Asoco niddhano pañño,",
  "anya-e1008n0:279": "Asoco paṇḍitabandhavo;",
  "anya-e1008n0:280": "Asocā vidhavā nārī,",
  "anya-e1008n0:281": "Puttanatta patiṭṭhitā.",
  "anya-e1008n0:282": "57.",
  "anya-e1008n0:283": "Avijjo puriso soco,",
  "anya-e1008n0:284": "Socaṃ methuna mappajaṃ;",
  "anya-e1008n0:285": "Nirāhārā pajā socā,",
  "anya-e1008n0:286": "Socaṃ rajja marājakaṃ.",
  "anya-e1008n0:287": "58.",
  "anya-e1008n0:288": "Kulehi saha sampakkaṃ,",
  "anya-e1008n0:289": "Paṇḍitehi ca mittataṃ;",
  "anya-e1008n0:290": "Ñātībhi ca samaṃ melaṃ,",
  "anya-e1008n0:291": "Kubbāno navinassati.",
  "anya-e1008n0:292": "59.",
  "anya-e1008n0:293": "Kaṭṭhā vutti parādhinā,",
  "anya-e1008n0:294": "Kaṭṭho vāso nirāsayo;",
  "anya-e1008n0:295": "Niddhano byavasāyo ca,",
  "anya-e1008n0:296": "Sabbakaṭṭhā daliddatā.",
  "anya-e1008n0:297": "60.",
  "anya-e1008n0:298": "Takkarassa kuto dhammo,",
  "anya-e1008n0:299": "Dujjanassa kuto khamā;",
  "anya-e1008n0:300": "Vesiyā ca kuto sneho,",
  "anya-e1008n0:301": "Kuto saccañca kāminaṃ.",
  "anya-e1008n0:302": "61.",
  "anya-e1008n0:303": "Pesitassa kuto mānaṃ,",
  "anya-e1008n0:304": "Kopanassa kuto sukhaṃ;",
  "anya-e1008n0:305": "Thīnaṃ kuto satittañca,",
  "anya-e1008n0:306": "Kuto mettī khalassa ca.",
  "anya-e1008n0:307": "62.",
  "anya-e1008n0:308": "Dubbalassa balaṃ rājā,",
  "anya-e1008n0:309": "Bālānaṃ rodanaṃ balaṃ;",
  "anya-e1008n0:310": "Balaṃmūḷhassa monittaṃ,",
  "anya-e1008n0:311": "Corānaṃ atathaṃ balaṃ.",
  "anya-e1008n0:312": "63.",
  "anya-e1008n0:313": "Yo dhuvāni pariccajja,",
  "anya-e1008n0:314": "Adhuvaṃ parisevati;",
  "anya-e1008n0:315": "Dhuvāni tassa nassanti,",
  "anya-e1008n0:316": "Adhuvaṃ naṭṭhameva ca.",
  "anya-e1008n0:317": "64.",
  "anya-e1008n0:318": "Sukkaṃ maṃsaṃ thiyo vuddhā,",
  "anya-e1008n0:319": "Bālakka taruṇaṃ dadhi;",
  "anya-e1008n0:320": "Pabhāte methunaṃ niddā,",
  "anya-e1008n0:321": "Sajju pāṇaharāni cha;",
  "anya-e1008n0:322": "65.",
  "anya-e1008n0:323": "Sajju maṃsaṃ navannañca,",
  "anya-e1008n0:324": "Bālā thī khīrabhojanaṃ;",
  "anya-e1008n0:325": "Ghatamuṇhodakañceva,",
  "anya-e1008n0:326": "Sajju pāṇakarāni cha.",
  "anya-e1008n0:327": "66.",
  "anya-e1008n0:328": "Sīhādekaṃ bakādekaṃ,",
  "anya-e1008n0:329": "Cha sunā tīṇi gadrabhā;",
  "anya-e1008n0:330": "Vāyasā catu sikkhetha,",
  "anya-e1008n0:331": "Cattāri kukkuṭādapi.",
  "anya-e1008n0:332": "67.",
  "anya-e1008n0:333": "Pabhūtamappakiccaṃ vā,",
  "anya-e1008n0:334": "Yonaro kattumicchati;",
  "anya-e1008n0:335": "Saṃyatanena kattabbaṃ,",
  "anya-e1008n0:336": "Sīhādekaṃ pakittitaṃ.",
  "anya-e1008n0:337": "68.",
  "anya-e1008n0:338": "Sabbindriyāni saṃyama,",
  "anya-e1008n0:339": "Bakova patito jano;",
  "anya-e1008n0:340": "Kāladesopapannāni,",
  "anya-e1008n0:341": "Sabbakiccāni sādhaye.",
  "anya-e1008n0:342": "69.",
  "anya-e1008n0:343": "Bahvāsī sāppasantuṭṭho,",
  "anya-e1008n0:344": "Suniddo sīghacetano;",
  "anya-e1008n0:345": "Pabhubhatto ca sūro ca,",
  "anya-e1008n0:346": "Ñātabbā cha sunā guṇā.",
  "anya-e1008n0:347": "70.",
  "anya-e1008n0:348": "Avissāmaṃ vahe bhāraṃ,",
  "anya-e1008n0:349": "Sītuṇhañca navindati;",
  "anya-e1008n0:350": "Sa santoso tathā niccaṃ,",
  "anya-e1008n0:351": "Tīṇi sikkhetha gadrabhā.",
  "anya-e1008n0:352": "71.",
  "anya-e1008n0:353": "Guḷhamethunadhammañca,",
  "anya-e1008n0:354": "Kālekāle ca saṅgahaṃ;",
  "anya-e1008n0:355": "Appamādamanālasyaṃ,",
  "anya-e1008n0:356": "Catu sikkhetha vāyasā.",
  "anya-e1008n0:357": "72.",
  "anya-e1008n0:358": "Yuddhañca pātaruṭṭhānaṃ,",
  "anya-e1008n0:359": "Bhojanaṃ saha bandhuhi;",
  "anya-e1008n0:360": "Thiyaṃ āpadaggataṃ rakkhe,",
  "anya-e1008n0:361": "Catu sikkhetha kukkuṭā.",
  "anya-e1008n0:362": "73.",
  "anya-e1008n0:363": "Kotibhāro samatthānaṃ,",
  "anya-e1008n0:364": "Kiṃdūraṃ byavasāyinaṃ;",
  "anya-e1008n0:365": "Ko videso savijjānaṃ,",
  "anya-e1008n0:366": "Ko paro piyavādinaṃ.",
  "anya-e1008n0:367": "74.",
  "anya-e1008n0:368": "Bhayassa kathito pantho,",
  "anya-e1008n0:369": "Indriyānamasaṃyamo;",
  "anya-e1008n0:370": "Tajjayo sampadāmaggo,",
  "anya-e1008n0:371": "Yeniṭṭhaṃ tena gamyate.",
  "anya-e1008n0:372": "75.",
  "anya-e1008n0:373": "Na ca vijjāsamo bandhu,",
  "anya-e1008n0:374": "Na ca byādhisamo ripu;",
  "anya-e1008n0:375": "Nacāpaccasamo sneho,",
  "anya-e1008n0:376": "Na ca devā paraṃ balaṃ.",
  "anya-e1008n0:377": "76.",
  "anya-e1008n0:378": "Samuddāvaraṇā bhūmi,",
  "anya-e1008n0:379": "Pākārāvaraṇaṃ gahaṃ;",
  "anya-e1008n0:380": "Narindāvaraṇā desā,",
  "anya-e1008n0:381": "Cārittāvaraṇā thiyo.",
  "anya-e1008n0:382": "77.",
  "anya-e1008n0:383": "Ghatakumbhasamā nārī,",
  "anya-e1008n0:384": "Tattaṅgāra samo pumā;",
  "anya-e1008n0:385": "Tasmā ghatañca aggiñca,",
  "anya-e1008n0:386": "Nekatra thapaye budho.",
  "anya-e1008n0:387": "78.",
  "anya-e1008n0:388": "Āhāro dviguṇo thīnaṃ,",
  "anya-e1008n0:389": "Buddhi tāsaṃ catugguṇo;",
  "anya-e1008n0:390": "Chaguṇo byavasāyo ca,",
  "anya-e1008n0:391": "Kāmocaṭṭhaguṇo mato.",
  "anya-e1008n0:392": "79.",
  "anya-e1008n0:393": "Jiṇṇamannaṃ pasaṃseyya,",
  "anya-e1008n0:394": "Bhariyaṃ gatayobbanaṃ;",
  "anya-e1008n0:395": "Raṇā paccāgataṃ sūraṃ,",
  "anya-e1008n0:396": "Sassañca gehamāgataṃ.",
  "anya-e1008n0:397": "80.",
  "anya-e1008n0:398": "Asantuṭṭhā dvijā naṭṭhā,",
  "anya-e1008n0:399": "Santuṭṭhāiva pāthivā;",
  "anya-e1008n0:400": "Salajjā gaṇikā naṭṭhā,",
  "anya-e1008n0:401": "Nillajjā ca kulitthiyo.",
  "anya-e1008n0:402": "81.",
  "anya-e1008n0:403": "Avaṃsapatito rājā,",
  "anya-e1008n0:404": "Mūḷhaputto ca paṇḍito;",
  "anya-e1008n0:405": "Adhanena dhanaṃ pāpya,",
  "anya-e1008n0:406": "Tiṇaṃva maññate janaṃ.",
  "anya-e1008n0:407": "82.",
  "anya-e1008n0:408": "Brahmahāpi naro pujjo,",
  "anya-e1008n0:409": "Yassatthi vipulaṃ dhanaṃ;",
  "anya-e1008n0:410": "Sasino tulyavaṃsopi,",
  "anya-e1008n0:411": "Niddhano paribhūyate.",
  "anya-e1008n0:412": "83.",
  "anya-e1008n0:413": "Potthakaṭṭhā tu yāvijjā,",
  "anya-e1008n0:414": "Parahatthagataṃ dhanaṃ;",
  "anya-e1008n0:415": "Kiccakāle samuppanne,",
  "anya-e1008n0:416": "Na sāvijjā na taddhanaṃ.",
  "anya-e1008n0:417": "84.",
  "anya-e1008n0:418": "Pādapānaṃ bhayaṃ vātā,",
  "anya-e1008n0:419": "Padmānaṃ sisirā bhayaṃ;",
  "anya-e1008n0:420": "Pabbatānaṃ vajīramhā,",
  "anya-e1008n0:421": "Sādhūnaṃ dujjanā bhayaṃ.",
  "anya-e1008n0:422": "85.",
  "anya-e1008n0:423": "Paññe niyujjamāne tu,",
  "anya-e1008n0:424": "Santi rañño tayoguṇā;",
  "anya-e1008n0:425": "Yaso sagganivāso ca,",
  "anya-e1008n0:426": "Vipulo ca dhanāgamo.",
  "anya-e1008n0:427": "86.",
  "anya-e1008n0:428": "Mūḷhe niyujjamānetu,",
  "anya-e1008n0:429": "Khattiyassāguṇā tayo;",
  "anya-e1008n0:430": "Ayaso catthanāso ca,",
  "anya-e1008n0:431": "Narake gamanaṃ tathā.",
  "anya-e1008n0:432": "87.",
  "anya-e1008n0:433": "Bahūmūḷhasaṅghātehi,",
  "anya-e1008n0:434": "Aññoññapasuvuttibhi;",
  "anya-e1008n0:435": "Pacchādyante guṇā sabbe,",
  "anya-e1008n0:436": "Meghehiva divākaro.",
  "anya-e1008n0:437": "88.",
  "anya-e1008n0:438": "Yassa khettaṃ nadītīre,",
  "anya-e1008n0:439": "Bhariyāpi parappiyā;",
  "anya-e1008n0:440": "Puttassa vinayo natthi,",
  "anya-e1008n0:441": "Maccureva nasaṃsayo.",
  "anya-e1008n0:442": "89.",
  "anya-e1008n0:443": "Asambhābyaṃ navattabbaṃ,",
  "anya-e1008n0:444": "Paccakkhamapi dissate;",
  "anya-e1008n0:445": "Silā tarati pānīyaṃ,",
  "anya-e1008n0:446": "Gītaṃ gāyati vānaro.",
  "anya-e1008n0:447": "90.",
  "anya-e1008n0:448": "Subhikkhaṃ kasake niccaṃ,",
  "anya-e1008n0:449": "Niccaṃ sukha marogike;",
  "anya-e1008n0:450": "Bhariyā bhattu piyā yassa,",
  "anya-e1008n0:451": "Tassa niccossavaṃ gahaṃ.",
  "anya-e1008n0:452": "91.",
  "anya-e1008n0:453": "Helassa kammanāsāya,",
  "anya-e1008n0:454": "Buddhināsāya niddhanaṃ;",
  "anya-e1008n0:455": "Yācanā mānanāsāya,",
  "anya-e1008n0:456": "Kulanāsāya bhojanaṃ.",
  "anya-e1008n0:457": "92.",
  "anya-e1008n0:458": "Sevitabbo mahāvakkho,",
  "anya-e1008n0:459": "Phalacchāyā samanvito;",
  "anya-e1008n0:460": "Yadi devā phalaṃ natthi,",
  "anya-e1008n0:461": "Chāyā kena nivāraye.",
  "anya-e1008n0:462": "93.",
  "anya-e1008n0:463": "Paṭhame najjitā vijjā,",
  "anya-e1008n0:464": "Dutīye najjitaṃ dhanaṃ;",
  "anya-e1008n0:465": "Tatīye najjitaṃ puññaṃ,",
  "anya-e1008n0:466": "Catutthe kiṃkarissati.",
  "anya-e1008n0:467": "94.",
  "anya-e1008n0:468": "Nadīkūleca ye vakkhā,",
  "anya-e1008n0:469": "Parahatthagataṃ dhanaṃ;",
  "anya-e1008n0:470": "Kiccaṃ thīgocaraṃ yassa,",
  "anya-e1008n0:471": "Sabbaṃ taṃ viphalaṃ bhave.",
  "anya-e1008n0:472": "95.",
  "anya-e1008n0:473": "Kudesamāsajja kutotthasañcayo,",
  "anya-e1008n0:474": "Kuputtamāsajja kuto jalañjalī;",
  "anya-e1008n0:475": "Kugehiniṃ pāpya gahe kuto sukhaṃ,",
  "anya-e1008n0:476": "Kusissamajjhāpayato kuto yaso.",
  "anya-e1008n0:477": "96.",
  "anya-e1008n0:478": "Kūpodakaṃ vaṭacchāyā,",
  "anya-e1008n0:479": "Sāmā thīciṭṭhakālayaṃ;",
  "anya-e1008n0:480": "Sītakāle bhave uṇhaṃ,",
  "anya-e1008n0:481": "Gimhakāle ca sītalaṃ.",
  "anya-e1008n0:482": "97.",
  "anya-e1008n0:483": "Visaṃ caṅkamanaṃ rattiṃ,",
  "anya-e1008n0:484": "Visaṃ raññonukulatā;",
  "anya-e1008n0:485": "Visaṃ thīpi aññāsattā,",
  "anya-e1008n0:486": "Visaṃ byādhi avekkhito.",
  "anya-e1008n0:487": "98.",
  "anya-e1008n0:488": "Duradhītā visaṃ vijjā,",
  "anya-e1008n0:489": "Ajiṇṇe bhojanaṃ visaṃ;",
  "anya-e1008n0:490": "Visaṃ goṭṭhī daliddassa,",
  "anya-e1008n0:491": "Vuddhassa taruṇī visaṃ.",
  "anya-e1008n0:492": "99.",
  "anya-e1008n0:493": "Padose nihato pantho,",
  "anya-e1008n0:494": "Patitā nihatā thiyo;",
  "anya-e1008n0:495": "Appabījaṃ hataṃ khettaṃ,",
  "anya-e1008n0:496": "Bhaccadosā hato pabhū.",
  "anya-e1008n0:497": "100.",
  "anya-e1008n0:498": "Hatamasottiyaṃ saddhaṃ,",
  "anya-e1008n0:499": "Hato yañño tvadakkhiṇo;",
  "anya-e1008n0:500": "Hatā rūpavatī vañjhā,",
  "anya-e1008n0:501": "Hataṃ senamanāyakaṃ.",
  "anya-e1008n0:502": "101.",
  "anya-e1008n0:503": "Vedavedaṅga tattañño,",
  "anya-e1008n0:504": "Japahomaparāyano;",
  "anya-e1008n0:505": "Āsīvādavacoyutto,",
  "anya-e1008n0:506": "Esa rājapurohito.",
  "anya-e1008n0:507": "102.",
  "anya-e1008n0:508": "Kulasīlaguṇopeto,",
  "anya-e1008n0:509": "Sabbadhammaparāyano;",
  "anya-e1008n0:510": "Pavīṇo pesanādyakkho,",
  "anya-e1008n0:511": "Dhammādyakkho vidhīyate.",
  "anya-e1008n0:512": "103.",
  "anya-e1008n0:513": "Ayubbedakatābhyāso,",
  "anya-e1008n0:514": "Sabbesaṃ piyadassano;",
  "anya-e1008n0:515": "Ariyasīlaguṇopeto,",
  "anya-e1008n0:516": "Esa vajjo vidhīyate.",
  "anya-e1008n0:517": "104.",
  "anya-e1008n0:518": "Sakiṃdutta gahitattho,",
  "anya-e1008n0:519": "Lahuhattho jitakkharo;",
  "anya-e1008n0:520": "Sabbasattha samālokī,",
  "anya-e1008n0:521": "Pakaṭṭho nāma lekhako.",
  "anya-e1008n0:522": "105.",
  "anya-e1008n0:523": "Samattanītisattañño,",
  "anya-e1008n0:524": "Vāhane pūjitassamo;",
  "anya-e1008n0:525": "Sūravīraguṇopeto,",
  "anya-e1008n0:526": "Senādhyakkho vidhīyate.",
  "anya-e1008n0:527": "106.",
  "anya-e1008n0:528": "Sucī vākyapaṭuppañño,",
  "anya-e1008n0:529": "Paracittopalakkhako;",
  "anya-e1008n0:530": "Dhīro yathāttha vādī ca,",
  "anya-e1008n0:531": "Esa dūto vidhīyate.",
  "anya-e1008n0:532": "107.",
  "anya-e1008n0:533": "Puttanatta guṇopeto,",
  "anya-e1008n0:534": "Satthañño piṭṭhapācako;",
  "anya-e1008n0:535": "Sūro ca kathinoceva,",
  "anya-e1008n0:536": "Sūpakāro sa uccate.",
  "anya-e1008n0:537": "108.",
  "anya-e1008n0:538": "Iṅgitā kāratattañño,",
  "anya-e1008n0:539": "Balavā piyadassano;",
  "anya-e1008n0:540": "Appamādī sadā dakkho,",
  "anya-e1008n0:541": "Patihāro sa uccate.",
  "anya-e1008n0:542": "109.",
  "anya-e1008n0:543": "Yassa natthi sayaṃ paññā,",
  "anya-e1008n0:544": "Satthaṃ tassa karoti kiṃ;",
  "anya-e1008n0:545": "Locanehi vihīnassa,",
  "anya-e1008n0:546": "Dappaṇo kiṃkarissati.",
  "anya-e1008n0:547": "110.",
  "anya-e1008n0:548": "Kiṃkarissanti vattāro,",
  "anya-e1008n0:549": "Sotaṃ yattha navijjate;",
  "anya-e1008n0:550": "Naggakapaṇake dese,",
  "anya-e1008n0:551": "Rajaṇo kiṃkarissati."
}