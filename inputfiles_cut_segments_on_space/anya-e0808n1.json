{
  "anya-e0808n1:0": "2. Mattāvuttiniddesa-dutiyapariccheda",
  "anya-e0808n1:1": "Gaṇaniyama",
  "anya-e0808n1:2": "17.",
  "anya-e0808n1:3": "Chaṭṭho’khilalahu,jo vā,",
  "anya-e0808n1:4": "Gayutā’ññe,cha’ggaṇā,na jo visame,;",
  "anya-e0808n1:5": "Ariyāya’ntaḍḍhe lo, chaṭṭho,’nte go,gaṇā cha’ññe.",
  "anya-e0808n1:6": "Yatiniyama",
  "anya-e0808n1:7": "18.",
  "anya-e0808n1:8": "Paṭhamaḍḍhe chaṭṭho ce,",
  "anya-e0808n1:9": "Sabbalahe,’tthā’dilahuni bhavatiyati;",
  "anya-e0808n1:10": "Tapparako,ntepi, sace, carimepi, bhavati catuttho’nte.",
  "anya-e0808n1:11": "19.Ariyāsāmaññaṁ ce, pubbo’dita lakkhaṇaṁ bhave yassā.",
  "anya-e0808n1:12": "20. Ādima’matha pādayugaṁ, yassā tyaṁ’sehi sā pathyā.",
  "anya-e0808n1:13": "21.",
  "anya-e0808n1:14": "Yattha gaṇattaya mullaṅghi,",
  "anya-e0808n1:15": "Yo’bhayatthā’dimo bhave vipulā.",
  "anya-e0808n1:16": "22. Garumajjhago jakāro, catutthako dutiyako capalā.",
  "anya-e0808n1:17": "23.",
  "anya-e0808n1:18": "Capalā’gatā’khilaṁ ce, dalā’dimaṁ lakkhaṇaṁ bhajati yassā;",
  "anya-e0808n1:19": "Pathyālakkhaṇa’maññaṁ, mukhacapalā nāma sā bhavati.",
  "anya-e0808n1:20": "24.",
  "anya-e0808n1:21": "Pathyāya lakkhaṇaṁ ce, paṭhamaḍḍhe lakkhaṇaṁ tu capalāya;",
  "anya-e0808n1:22": "Dutiye dale’tha yassā, pakittitā sā jaghanacapalā.",
  "anya-e0808n1:23": "Ariyājātiyo.",
  "anya-e0808n1:24": "25.",
  "anya-e0808n1:25": "Sabbaṁpaṭhamadale yadi, lakkhaṇa’mariyāya vutta’mubhayesu;",
  "anya-e0808n1:26": "Yassā dalesu yuttaṁ,",
  "anya-e0808n1:27": "Vuttā sā gīti vutta yati lalitā.",
  "anya-e0808n1:28": "26.",
  "anya-e0808n1:29": "Ariyāyaṁ dutiya’ḍḍhe, gaditā’khilalakkhaṇaṁ yaṁ taṁ;",
  "anya-e0808n1:30": "Bhavati dalesu’bhayesupi,",
  "anya-e0808n1:31": "Yadi yassā sā’ya mupagīti.",
  "anya-e0808n1:32": "27.",
  "anya-e0808n1:33": "Ariyāya’ḍḍhadvitayaṁ, pubbodita lakkhaṇo’petaṁ;",
  "anya-e0808n1:34": "Vipariyayenā’bhihitaṁ,",
  "anya-e0808n1:35": "Yassā sambhavati ce’ha so’ggīti.",
  "anya-e0808n1:36": "28.",
  "anya-e0808n1:37": "Ariyāpubba’ḍḍhaṁ yadi, garune’kenā’dhikena nidhane yuttaṁ;",
  "anya-e0808n1:38": "Yadi pubba’ḍḍhasamānaṁ, dala mitaraṁ co’ditā’ya’mariyāgīti.",
  "anya-e0808n1:39": "Gītijātiyo.",
  "anya-e0808n1:40": "29.",
  "anya-e0808n1:41": "Visame cha siyuṁ kalā mukhe,",
  "anya-e0808n1:42": "Same tva’ṭṭha, ra,la,gā, tato’pari;",
  "anya-e0808n1:43": "Vetālīyaṁ ta muccate, lahu chakkaṁ na nirantaraṁ same.",
  "anya-e0808n1:44": "30.",
  "anya-e0808n1:45": "Vetālīyopamaṁ mukhe taṁ,",
  "anya-e0808n1:46": "Opacchandasakaṁ ra,yā ya’dante.",
  "anya-e0808n1:47": "31.Āpātalikā kathitā’yaṁ, bhagagā’nte yadi pubbamiva’ññaṁ.",
  "anya-e0808n1:48": "32.",
  "anya-e0808n1:49": "Yadā’dito dakkhiṇantikā,",
  "anya-e0808n1:50": "Ṭhite’ttha pādesvā’khilesu jo.",
  "anya-e0808n1:51": "33.",
  "anya-e0808n1:52": "‘Udiccavuttī’ti vuccate,",
  "anya-e0808n1:53": "Jo cā’do visamesu saṇṭhito.",
  "anya-e0808n1:54": "34. Pubbattha, samesu ce ga, jā, ‘paccavutti’ ruditā’ti saṇṭhitā.",
  "anya-e0808n1:55": "35.",
  "anya-e0808n1:56": "Samāsamā’trā’dinaṁ samā,",
  "anya-e0808n1:57": "Saṁyutā bhavati taṁ pavattakaṁ.",
  "anya-e0808n1:58": "36. Assa sā sama katā’ parantikā.",
  "anya-e0808n1:59": "37. Tada’ññajā cāruhāsinī.",
  "anya-e0808n1:60": "Vetālīyajātiyo.",
  "anya-e0808n1:61": "38. Dvika vihata vasu lahu acaladhiti ri’ha.",
  "anya-e0808n1:62": "39.Mattāsamakaṁ navamo lga’nte.",
  "anya-e0808n1:63": "40. Jo nlā’ thavā’ṇṇavā visiloko.",
  "anya-e0808n1:64": "41. Tadvayato vānavāsikā’khyā.",
  "anya-e0808n1:65": "42. Pañca,ṭṭha,navasu yadi lo citrā.",
  "anya-e0808n1:66": "43. Ga,lyā’ṭṭhahi ce’sā vu’pacitrā.",
  "anya-e0808n1:67": "44.",
  "anya-e0808n1:68": "Ya’matīta lakkhaṇa visesa yutaṁ, (citrā)",
  "anya-e0808n1:69": "Mattā samā’di pādā’bhihitaṁ; (Visiloka)",
  "anya-e0808n1:70": "Aniyata vutta parimāṇa sahitaṁ, (vānavāsikā)",
  "anya-e0808n1:71": "Pathitaṁ janesu pādākulakaṁ. (visiloka)",
  "anya-e0808n1:72": "Mattāsamaka jātiyo.",
  "anya-e0808n1:73": "45.",
  "anya-e0808n1:74": "Vinā vaṇṇehi mattā gā, vinā vaṇṇā garūhi tu;",
  "anya-e0808n1:75": "Vinā lahūhi garavo, dale pathyādino matā.",
  "anya-e0808n1:76": "Iti vuttodaye chandasi mattāvuttiniddeso nāma",
  "anya-e0808n1:77": "Dutiyo paricchedo."
}