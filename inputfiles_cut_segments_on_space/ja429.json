{
  "ja429:0.1": "Jātaka ",
  "ja429:0.2": "Navakanipāta ",
  "ja429:0.3": "Gijjhavagga ",
  "ja429:0.4": "3. Mahāsuvajātaka ",
  "ja429:1.1": "“Dumo yadā hoti phalūpapanno, ",
  "ja429:1.2": "Bhuñjanti naṁ vihaṅgamā sampatantā; ",
  "ja429:1.3": "Khīṇanti ñatvāna dumaṁ phalaccaye, ",
  "ja429:1.4": "Disodisaṁ yanti tato vihaṅgamā. ",
  "ja429:2.1": "Cara cārikaṁ lohitatuṇḍa māmari, ",
  "ja429:2.2": "Kiṁ tvaṁ suva sukkhadumamhi jhāyasi; ",
  "ja429:2.3": "Tadiṅgha maṁ brūhi vasantasannibha, ",
  "ja429:2.4": "Kasmā suva sukkhadumaṁ na riñcasi”. ",
  "ja429:3.1": "“Ye ve sakhīnaṁ sakhāro bhavanti, ",
  "ja429:3.2": "Pāṇaccaye dukkhasukhesu haṁsa; ",
  "ja429:3.3": "Khīṇaṁ akhīṇampi na taṁ jahanti, ",
  "ja429:3.4": "Santo sataṁ dhammamanussarantā. ",
  "ja429:4.1": "Sohaṁ sataṁ aññatarosmi haṁsa, ",
  "ja429:4.2": "Ñātī ca me hoti sakhā ca rukkho; ",
  "ja429:4.3": "Taṁ nussahe jīvikattho pahātuṁ, ",
  "ja429:4.4": "Khīṇanti ñatvāna na hesa dhammo”. ",
  "ja429:5.1": "“Sādhu sakkhi kataṁ hoti, ",
  "ja429:5.2": "metti saṁsati santhavo; ",
  "ja429:5.3": "Sacetaṁ dhammaṁ rocesi, ",
  "ja429:5.4": "pāsaṁsosi vijānataṁ. ",
  "ja429:6.1": "So te suva varaṁ dammi, ",
  "ja429:6.2": "pattayāna vihaṅgama; ",
  "ja429:6.3": "Varaṁ varassu vakkaṅga, ",
  "ja429:6.4": "yaṁ kiñci manasicchasi”. ",
  "ja429:7.1": "“Varañca me haṁsa bhavaṁ dadeyya, ",
  "ja429:7.2": "Ayañca rukkho punarāyuṁ labhetha; ",
  "ja429:7.3": "So sākhavā phalimā saṁvirūḷho, ",
  "ja429:7.4": "Madhutthiko tiṭṭhatu sobhamāno”. ",
  "ja429:8.1": "“Taṁ passa samma phalimaṁ uḷāraṁ, ",
  "ja429:8.2": "Sahāva te hotu udumbarena; ",
  "ja429:8.3": "So sākhavā phalimā saṁvirūḷho, ",
  "ja429:8.4": "Madhutthiko tiṭṭhatu sobhamāno”. ",
  "ja429:9.1": "“Evaṁ sakka sukhī hohi, ",
  "ja429:9.2": "saha sabbehi ñātibhi; ",
  "ja429:9.3": "Yathāhamajja sukhito, ",
  "ja429:9.4": "disvāna saphalaṁ dumaṁ”. ",
  "ja429:10.1": "“Suvassa ca varaṁ datvā, ",
  "ja429:10.2": "katvāna saphalaṁ dumaṁ; ",
  "ja429:10.3": "Pakkāmi saha bhariyāya, ",
  "ja429:10.4": "devānaṁ nandanaṁ vanan”ti. ",
  "ja429:11.1": "Mahāsuvajātakaṁ tatiyaṁ. "
}