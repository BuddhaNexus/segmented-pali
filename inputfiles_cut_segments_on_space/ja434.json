{
  "ja434:0.1": "Jātaka ",
  "ja434:0.2": "Navakanipāta ",
  "ja434:0.3": "Gijjhavagga ",
  "ja434:0.4": "8. Cakkavākajātaka ",
  "ja434:1.1": "“Kāsāyavatthe sakuṇe vadāmi, ",
  "ja434:1.2": "Duve duve nandamane carante; ",
  "ja434:1.3": "Kaṁ aṇḍajaṁ aṇḍajā mānusesu, ",
  "ja434:1.4": "Jātiṁ pasaṁsanti tadiṅgha brūtha”. ",
  "ja434:2.1": "“Amhe manussesu manussahiṁsa, ",
  "ja434:2.2": "Anubbate cakkavāke vadanti; ",
  "ja434:2.3": "Kalyāṇabhāvamhe dijesu sammatā, ",
  "ja434:2.4": "Abhirūpā vicarāma aṇṇave”. ",
  "ja434:2.5": "(…) ",
  "ja434:3.1": "“Kiṁ aṇṇave kāni phalāni bhuñje, ",
  "ja434:3.2": "Maṁsaṁ kuto khādatha cakkavākā; ",
  "ja434:3.3": "Kiṁ bhojanaṁ bhuñjatha vo anomā, ",
  "ja434:3.4": "Balañca vaṇṇo ca anapparūpā”. ",
  "ja434:4.1": "“Na aṇṇave santi phalāni dhaṅka, ",
  "ja434:4.2": "Maṁsaṁ kuto khādituṁ cakkavāke; ",
  "ja434:4.3": "Sevālabhakkhamha apāṇabhojanā, ",
  "ja434:4.4": "Na ghāsahetūpi karoma pāpaṁ”. ",
  "ja434:5.1": "“Na me idaṁ ruccati cakkavāka, ",
  "ja434:5.2": "Asmiṁ bhave bhojanasannikāso; ",
  "ja434:5.3": "Ahosi pubbe tato me aññathā, ",
  "ja434:5.4": "Icceva me vimati ettha jātā. ",
  "ja434:6.1": "Ahampi maṁsāni phalāni bhuñje, ",
  "ja434:6.2": "Annāni ca loṇiyateliyāni; ",
  "ja434:6.3": "Rasaṁ manussesu labhāmi bhottuṁ, ",
  "ja434:6.4": "Sūrova saṅgāmamukhaṁ vijetvā; ",
  "ja434:6.5": "Na ca me tādiso vaṇṇo, ",
  "ja434:6.6": "Cakkavāka yathā tava”. ",
  "ja434:7.1": "“Asuddhabhakkhosi khaṇānupātī, ",
  "ja434:7.2": "Kicchena te labbhati annapānaṁ; ",
  "ja434:7.3": "Na tussasī rukkhaphalehi dhaṅka, ",
  "ja434:7.4": "Maṁsāni vā yāni susānamajjhe. ",
  "ja434:8.1": "Yo sāhasena adhigamma bhoge, ",
  "ja434:8.2": "Paribhuñjati dhaṅka khaṇānupātī; ",
  "ja434:8.3": "Tato upakkosati naṁ sabhāvo, ",
  "ja434:8.4": "Upakkuṭṭho vaṇṇabalaṁ jahāti. ",
  "ja434:9.1": "Appampi ce nibbutiṁ bhuñjatī yadi, ",
  "ja434:9.2": "Asāhasena aparūpaghātī; ",
  "ja434:9.3": "Balañca vaṇṇo ca tadassa hoti, ",
  "ja434:9.4": "Na hi sabbo āhāramayena vaṇṇo”ti. ",
  "ja434:10.1": "Cakkavākajātakaṁ aṭṭhamaṁ. "
}