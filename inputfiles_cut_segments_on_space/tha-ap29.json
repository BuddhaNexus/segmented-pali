{
  "tha-ap29:0.1": "Therāpadāna ",
  "tha-ap29:0.2": "Subhūtivagga ",
  "tha-ap29:0.3": "7 Pulinapūjakattheraapadāna ",
  "tha-ap29:1.1": "“Vipassissa bhagavato, ",
  "tha-ap29:1.2": "bodhiyā pādaputtame; ",
  "tha-ap29:1.3": "Purāṇapulinaṁ hitvā, ",
  "tha-ap29:1.4": "suddhapulinamākiriṁ. ",
  "tha-ap29:2.1": "Ekanavutito kappe, ",
  "tha-ap29:2.2": "yaṁ pulinamadāsahaṁ; ",
  "tha-ap29:2.3": "Duggatiṁ nābhijānāmi, ",
  "tha-ap29:2.4": "pulinadānassidaṁ phalaṁ. ",
  "tha-ap29:3.1": "Tiṁsatime ito kappe, ",
  "tha-ap29:3.2": "rājā āsiṁ janādhibhū; ",
  "tha-ap29:3.3": "Mahāpulinanāmena, ",
  "tha-ap29:3.4": "cakkavattī mahabbalo. ",
  "tha-ap29:4.1": "Paṭisambhidā catasso, ",
  "tha-ap29:4.2": "vimokkhāpi ca aṭṭhime; ",
  "tha-ap29:4.3": "Chaḷabhiññā sacchikatā, ",
  "tha-ap29:4.4": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap29:5.1": "Itthaṁ sudaṁ āyasmā pulinapūjako thero imā gāthāyo abhāsitthāti. ",
  "tha-ap29:6.1": "Pulinapūjakattherassāpadānaṁ sattamaṁ. "
}