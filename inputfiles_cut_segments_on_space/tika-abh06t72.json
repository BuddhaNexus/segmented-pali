{
  "tika-abh06t72:0.1": "Saccasaṅkhepo",
  "tika-abh06t72:0.2": "Ganthārambhakathā",
  "tika-abh06t72:1": "1.",
  "tika-abh06t72:2": " Namassitvā tilokaggaṁ, ñeyyasāgarapāraguṁ;",
  "tika-abh06t72:3": "Bhavābhāvakaraṁ dhammaṁ, gaṇañca guṇasāgaraṁ.",
  "tika-abh06t72:4": "2.",
  "tika-abh06t72:5": " Nissāya pubbācariyamataṁ atthāvirodhinaṁ;",
  "tika-abh06t72:6": "Vakkhāmi saccasaṅkhepaṁ, hitaṁ kārakayoginaṁ."
}