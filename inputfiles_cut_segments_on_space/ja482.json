{
  "ja482:0.1": "Jātaka ",
  "ja482:0.2": "Terasakanipāta ",
  "ja482:0.3": "Ambavagga ",
  "ja482:0.4": "9. Rurumigarājajātaka ",
  "ja482:1.1": "“Tassa gāmavaraṁ dammi, ",
  "ja482:1.2": "Nāriyo ca alaṅkatā; ",
  "ja482:1.3": "Yo metaṁ migamakkhāti, ",
  "ja482:1.4": "Migānaṁ migamuttamaṁ”. ",
  "ja482:2.1": "“Mayhaṁ gāmavaraṁ dehi, ",
  "ja482:2.2": "nāriyo ca alaṅkatā; ",
  "ja482:2.3": "Ahaṁ te migamakkhissaṁ, ",
  "ja482:2.4": "migānaṁ migamuttamaṁ. ",
  "ja482:3.1": "Etasmiṁ vanasaṇḍasmiṁ, ",
  "ja482:3.2": "ambā sālā ca pupphitā; ",
  "ja482:3.3": "Indagopakasañchannā, ",
  "ja482:3.4": "ettheso tiṭṭhate migo”. ",
  "ja482:4.1": "“Dhanuṁ advejjhaṁ katvāna, ",
  "ja482:4.2": "usuṁ sannayhupāgami; ",
  "ja482:4.3": "Migo ca disvā rājānaṁ, ",
  "ja482:4.4": "dūrato ajjhabhāsatha. ",
  "ja482:5.1": "Āgamehi mahārāja, ",
  "ja482:5.2": "mā maṁ vijjhi rathesabha; ",
  "ja482:5.3": "Ko nu te idamakkhāsi, ",
  "ja482:5.4": "ettheso tiṭṭhate migo”. ",
  "ja482:6.1": "“Esa pāpacaro poso, ",
  "ja482:6.2": "samma tiṭṭhati ārakā; ",
  "ja482:6.3": "Soyaṁ me idamakkhāsi, ",
  "ja482:6.4": "ettheso tiṭṭhate migo”. ",
  "ja482:7.1": "“Saccaṁ kireva māhaṁsu, ",
  "ja482:7.2": "narā ekacciyā idha; ",
  "ja482:7.3": "Kaṭṭhaṁ niplavitaṁ seyyo, ",
  "ja482:7.4": "na tvevekacciyo naro”. ",
  "ja482:8.1": "“Kiṁ nu ruru garahasi migānaṁ, ",
  "ja482:8.2": "Kiṁ pakkhīnaṁ kiṁ pana mānusānaṁ; ",
  "ja482:8.3": "Bhayañhi maṁ vindatinapparūpaṁ, ",
  "ja482:8.4": "Sutvāna taṁ mānusiṁ bhāsamānaṁ”. ",
  "ja482:9.1": "“Yamuddhariṁ vāhane vuyhamānaṁ, ",
  "ja482:9.2": "Mahodake salile sīghasote; ",
  "ja482:9.3": "Tatonidānaṁ bhayamāgataṁ mama, ",
  "ja482:9.4": "Dukkho have rāja asabbhi saṅgamo”. ",
  "ja482:10.1": "“Sohaṁ catuppattamimaṁ vihaṅgamaṁ, ",
  "ja482:10.2": "Tanucchidaṁ hadaye ossajāmi; ",
  "ja482:10.3": "Hanāmi taṁ mittadubbhiṁ akiccakāriṁ, ",
  "ja482:10.4": "Yo tādisaṁ kammakataṁ na jāne”. ",
  "ja482:11.1": "“Dhīrassa bālassa have janinda, ",
  "ja482:11.2": "Santo vadhaṁ nappasaṁsanti jātu; ",
  "ja482:11.3": "Kāmaṁ gharaṁ gacchatu pāpadhammo, ",
  "ja482:11.4": "Yañcassa bhaṭṭhaṁ tadetassa dehi; ",
  "ja482:11.5": "Ahañca te kāmakaro bhavāmi”. ",
  "ja482:12.1": "“Addhā rurū aññataro sataṁ so, ",
  "ja482:12.2": "Yo dubbhato mānusassa na dubbhi; ",
  "ja482:12.3": "Kāmaṁ gharaṁ gacchatu pāpadhammo, ",
  "ja482:12.4": "Yañcassa bhaṭṭhaṁ tadetassa dammi; ",
  "ja482:12.5": "Ahañca te kāmacāraṁ dadāmi”. ",
  "ja482:13.1": "“Suvijānaṁ siṅgālānaṁ, ",
  "ja482:13.2": "sakuṇānañca vassitaṁ; ",
  "ja482:13.3": "Manussavassitaṁ rāja, ",
  "ja482:13.4": "dubbijānataraṁ tato. ",
  "ja482:14.1": "Api ce maññatī poso, ",
  "ja482:14.2": "ñāti mitto sakhāti vā; ",
  "ja482:14.3": "Yo pubbe sumano hutvā, ",
  "ja482:14.4": "pacchā sampajjate diso”. ",
  "ja482:15.1": "“Samāgatā jānapadā, ",
  "ja482:15.2": "negamā ca samāgatā; ",
  "ja482:15.3": "Migā sassāni khādanti, ",
  "ja482:15.4": "taṁ devo paṭisedhatu”. ",
  "ja482:16.1": "“Kāmaṁ janapado māsi, ",
  "ja482:16.2": "raṭṭhañcāpi vinassatu; ",
  "ja482:16.3": "Na tvevāhaṁ ruruṁ dubbhe, ",
  "ja482:16.4": "datvā abhayadakkhiṇaṁ. ",
  "ja482:17.1": "Mā me janapado āsi, ",
  "ja482:17.2": "raṭṭhañcāpi vinassatu; ",
  "ja482:17.3": "Na tvevāhaṁ migarājassa, ",
  "ja482:17.4": "varaṁ datvā musā bhaṇe”ti. ",
  "ja482:18.1": "Rurumigarājajātakaṁ navamaṁ. "
}