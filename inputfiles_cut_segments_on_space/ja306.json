{
  "ja306:0.1": "Jātaka ",
  "ja306:0.2": "Catukkanipāta ",
  "ja306:0.3": "Kāliṅgavagga ",
  "ja306:0.4": "6. Sujātājātaka ",
  "ja306:1.1": "“Kimaṇḍakā ime deva, ",
  "ja306:1.2": "nikkhittā kaṁsamallake; ",
  "ja306:1.3": "Upalohitakā vaggū, ",
  "ja306:1.4": "taṁ me akkhāhi pucchito”. ",
  "ja306:2.1": "“Yāni pure tuvaṁ devi, ",
  "ja306:2.2": "bhaṇḍu nantakavāsinī; ",
  "ja306:2.3": "Ucchaṅgahatthā pacināsi, ",
  "ja306:2.4": "tassā te koliyaṁ phalaṁ. ",
  "ja306:3.1": "Uḍḍayhate na ramati, ",
  "ja306:3.2": "bhogā vippajahanti taṁ; ",
  "ja306:3.3": "Tatthevimaṁ paṭinetha, ",
  "ja306:3.4": "yattha kolaṁ pacissati”. ",
  "ja306:4.1": "“Honti hete mahārāja, ",
  "ja306:4.2": "iddhippattāya nāriyā; ",
  "ja306:4.3": "Khama deva sujātāya, ",
  "ja306:4.4": "māssā kujjha rathesabhā”ti. ",
  "ja306:5.1": "Sujātājātakaṁ chaṭṭhaṁ. "
}