{
  "anya-e0603n1:0": "Buddhavandanā",
  "anya-e0603n1:1": "1.",
  "anya-e0603n1:2": " Devalokā cavitvāna ,",
  "anya-e0603n1:3": "Mahāmāyāya kucchiyaṁ;",
  "anya-e0603n1:4": "Uppajji guruvāramhi,",
  "anya-e0603n1:5": "Vandetaṁ sakyapuṅgavaṁ.",
  "anya-e0603n1:6": "2.",
  "anya-e0603n1:7": " Dasamā saccaye neso ,",
  "anya-e0603n1:8": "Vijāyi mātukucchito;",
  "anya-e0603n1:9": "Sukkavāre lumbiniyaṁ,",
  "anya-e0603n1:10": "Vande taṁ lokapūjitaṁ.",
  "anya-e0603n1:11": "3.",
  "anya-e0603n1:12": " Cakkavattisiriṁ hitvā,",
  "anya-e0603n1:13": "Mahāsinikkhamaṁ sudhī;",
  "anya-e0603n1:14": "Nikkhamī candavāramhi,",
  "anya-e0603n1:15": "Name taṁ munikuñjaraṁ.",
  "anya-e0603n1:16": "4.",
  "anya-e0603n1:17": " Assatthabodhimūlamhi ,",
  "anya-e0603n1:18": "Pallaṅke aparājite;",
  "anya-e0603n1:19": "Patto sabbaññutaṁ nātho,",
  "anya-e0603n1:20": "Name taṁ budhavāsare.",
  "anya-e0603n1:21": "5.",
  "anya-e0603n1:22": " Pañcavaggiyamukhānaṁ ,",
  "anya-e0603n1:23": "Devānaṁ migadāvane;",
  "anya-e0603n1:24": "Sorivāre dhammacakkaṁ,",
  "anya-e0603n1:25": "Vattesi taṁ jinaṁ name.",
  "anya-e0603n1:26": "6.",
  "anya-e0603n1:27": " Sabbasaṅkhatadhammānaṁ,",
  "anya-e0603n1:28": "Pakāsiyaaniccataṁ;",
  "anya-e0603n1:29": "Nibbutaṁaṅgavāramhi,",
  "anya-e0603n1:30": "Namādhiamatandadaṁ.",
  "anya-e0603n1:31": "7.",
  "anya-e0603n1:32": " Sūriyavāre soṇṇāya ,",
  "anya-e0603n1:33": "Doṇiyā jinaviggaho;",
  "anya-e0603n1:34": "Agginujjalito tassa,",
  "anya-e0603n1:35": "Name dhātusarīrakaṁ."
}