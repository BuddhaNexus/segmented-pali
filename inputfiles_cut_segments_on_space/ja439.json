{
  "ja439:0.1": "Jātaka ",
  "ja439:0.2": "Dasakanipāta ",
  "ja439:0.3": "Catudvāravagga ",
  "ja439:0.4": "1. Catudvārajātaka ",
  "ja439:1.1": "“Catudvāramidaṁ nagaraṁ, ",
  "ja439:1.2": "āyasaṁ daḷhapākāraṁ; ",
  "ja439:1.3": "Oruddhapaṭiruddhosmi, ",
  "ja439:1.4": "kiṁ pāpaṁ pakataṁ mayā”. ",
  "ja439:2.1": "“Sabbe apihitā dvārā, ",
  "ja439:2.2": "oruddhosmi yathā dijo; ",
  "ja439:2.3": "Kimādhikaraṇaṁ yakkha, ",
  "ja439:2.4": "cakkābhinihato ahaṁ. ",
  "ja439:3.1": "Laddhā satasahassāni, ",
  "ja439:3.2": "atirekāni vīsati; ",
  "ja439:3.3": "Anukampakānaṁ ñātīnaṁ, ",
  "ja439:3.4": "vacanaṁ samma nākari. ",
  "ja439:4.1": "Laṅghiṁ samuddaṁ pakkhandi, ",
  "ja439:4.2": "sāgaraṁ appasiddhikaṁ; ",
  "ja439:4.3": "Catubbhi aṭṭhajjhagamā, ",
  "ja439:4.4": "aṭṭhāhipi ca soḷasa. ",
  "ja439:5.1": "Soḷasāhi ca bāttiṁsa, ",
  "ja439:5.2": "atricchaṁ cakkamāsado; ",
  "ja439:5.3": "Icchāhatassa posassa, ",
  "ja439:5.4": "cakkaṁ bhamati matthake. ",
  "ja439:6.1": "Uparivisālā duppūrā, ",
  "ja439:6.2": "icchā visaṭagāminī; ",
  "ja439:6.3": "Ye ca taṁ anugijjhanti, ",
  "ja439:6.4": "te honti cakkadhārino. ",
  "ja439:7.1": "Bahubhaṇḍaṁ avahāya, ",
  "ja439:7.2": "maggaṁ appaṭivekkhiya; ",
  "ja439:7.3": "Yesañcetaṁ asaṅkhātaṁ, ",
  "ja439:7.4": "te honti cakkadhārino. ",
  "ja439:8.1": "Kammaṁ samekkhe vipulañca bhogaṁ, ",
  "ja439:8.2": "Icchaṁ na seveyya anatthasaṁhitaṁ; ",
  "ja439:8.3": "Kareyya vākyaṁ anukampakānaṁ, ",
  "ja439:8.4": "Taṁ tādisaṁ nātivatteyya cakkaṁ”. ",
  "ja439:9.1": "“Kīvaciraṁ nu me yakkha, ",
  "ja439:9.2": "cakkaṁ sirasi ṭhassati; ",
  "ja439:9.3": "Kati vassasahassāni, ",
  "ja439:9.4": "taṁ me akkhāhi pucchito”. ",
  "ja439:10.1": "“Atisaro paccasaro, ",
  "ja439:10.2": "mittavinda suṇohi me; ",
  "ja439:10.3": "Cakkaṁ te sirasi māviddhaṁ, ",
  "ja439:10.4": "na taṁ jīvaṁ pamokkhasī”ti. ",
  "ja439:11.1": "Catudvārajātakaṁ paṭhamaṁ. "
}