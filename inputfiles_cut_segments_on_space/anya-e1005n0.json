{
  "anya-e1005n0:0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1005n0:1": "Lokanīti",
  "anya-e1005n0:2": "1. Paṇḍitakaṇḍo",
  "anya-e1005n0:3": "1.",
  "anya-e1005n0:4": "Lokanītiṃ pavakkhāmi,",
  "anya-e1005n0:5": "Nānāsatthasamuddhaṭaṃ;",
  "anya-e1005n0:6": "Māgadheneva saṅkhepaṃ,",
  "anya-e1005n0:7": "Vanditvā ratanatthayaṃ.",
  "anya-e1005n0:8": "2.",
  "anya-e1005n0:9": "Nītiloke purisassa sāro,",
  "anya-e1005n0:10": "Mātā pitā ācariyoca mitto;",
  "anya-e1005n0:11": "Tasmā hi nītiṃ puriso vijaññā,",
  "anya-e1005n0:12": "Ñāṇī mahā hoti bahussutoca.",
  "anya-e1005n0:13": "3.",
  "anya-e1005n0:14": "Alasassa kuto sippaṃ,",
  "anya-e1005n0:15": "Asippassa kuto dhanaṃ;",
  "anya-e1005n0:16": "Adhanassa kuto mittaṃ,",
  "anya-e1005n0:17": "Amittassa kuto sukhaṃ;",
  "anya-e1005n0:18": "Asukhassa kuto puññaṃ,",
  "anya-e1005n0:19": "Apuññassa kuto varaṃ.",
  "anya-e1005n0:20": "4.",
  "anya-e1005n0:21": "Sippā samaṃ dhanaṃ natthi,",
  "anya-e1005n0:22": "Sippaṃ corā nagaṇhare;",
  "anya-e1005n0:23": "Idha loke sippaṃ mittaṃ,",
  "anya-e1005n0:24": "Paraloke sukhāvahaṃ.",
  "anya-e1005n0:25": "5.",
  "anya-e1005n0:26": "Appakaṃ nātimaññeyya,",
  "anya-e1005n0:27": "Citte sutaṃ nidhāpaye;",
  "anya-e1005n0:28": "Vammikodakabindūva,",
  "anya-e1005n0:29": "Cirena paripūrati.",
  "anya-e1005n0:30": "6.",
  "anya-e1005n0:31": "Khuddoti nātimaññeyya,",
  "anya-e1005n0:32": "Vijjaṃ vā sippameva vā;",
  "anya-e1005n0:33": "Ekampi pariyodātaṃ,",
  "anya-e1005n0:34": "Jīvitakappakāraṇaṃ.",
  "anya-e1005n0:35": "7.",
  "anya-e1005n0:36": "Sele sele na māṇikaṃ,",
  "anya-e1005n0:37": "Gaje gaje na muttikaṃ;",
  "anya-e1005n0:38": "Vane vane na candanaṃ,",
  "anya-e1005n0:39": "Ṭhāne ṭhāne na paṇḍitaṃ.",
  "anya-e1005n0:40": "8.",
  "anya-e1005n0:41": "Paṇḍito sutasampanno,",
  "anya-e1005n0:42": "Yattha atthīti ce suto;",
  "anya-e1005n0:43": "Mahussāhena taṃ ṭhānaṃ,",
  "anya-e1005n0:44": "Gantabbaṃva sutesinā.",
  "anya-e1005n0:45": "9.",
  "anya-e1005n0:46": "Sine sippaṃ sine dhanaṃ,",
  "anya-e1005n0:47": "Sine pabbatamāruhaṃ;",
  "anya-e1005n0:48": "Sine kāmassa kodhassa,",
  "anya-e1005n0:49": "Ime pañca sine sine.",
  "anya-e1005n0:50": "10.",
  "anya-e1005n0:51": "Suti sammuti saṅkhyāca,",
  "anya-e1005n0:52": "Yogā nīti visesakā;",
  "anya-e1005n0:53": "Gandhabbā gaṇikā ceva,",
  "anya-e1005n0:54": "Dhanu bedā ca pūraṇā.",
  "anya-e1005n0:55": "11.",
  "anya-e1005n0:56": "Tikicchā itihāsā ca,",
  "anya-e1005n0:57": "Joti māyā ca chandati;",
  "anya-e1005n0:58": "Ketu mantā ca saddā ca,",
  "anya-e1005n0:59": "Sippāṭṭhārasakā ime.",
  "anya-e1005n0:60": "12.",
  "anya-e1005n0:61": "Apuṭṭho paṇḍito bherī,",
  "anya-e1005n0:62": "Pajjunno me hoti pucchito;",
  "anya-e1005n0:63": "Bālo puṭṭho apuṭṭhopi,",
  "anya-e1005n0:64": "Bahumpi bhaṇate sadā.",
  "anya-e1005n0:65": "13.",
  "anya-e1005n0:66": "Potthakesu ca yaṃ sippaṃ,",
  "anya-e1005n0:67": "Parahatthesu yaṃ dhanaṃ;",
  "anya-e1005n0:68": "Yathākicce samuppanne,",
  "anya-e1005n0:69": "Na taṃ sippaṃ na taṃ dhanaṃ.",
  "anya-e1005n0:70": "14.",
  "anya-e1005n0:71": "Jalappamāṇaṃ kumuddanālaṃ,",
  "anya-e1005n0:72": "Kulappamāṇaṃ vinayo pamāṇaṃ;",
  "anya-e1005n0:73": "Byattippamāṇaṃ kathītavākyaṃ,",
  "anya-e1005n0:74": "Pathaviyā pamāṇaṃ tiṇa milātaṃ.",
  "anya-e1005n0:75": "15.",
  "anya-e1005n0:76": "Appassuto sutaṃ appaṃ,",
  "anya-e1005n0:77": "Bahuṃ maññati mānavā;",
  "anya-e1005n0:78": "Sindhūdakaṃ apassanto,",
  "anya-e1005n0:79": "Kūpe toyaṃva maṇḍuko.",
  "anya-e1005n0:80": "16.",
  "anya-e1005n0:81": "Pathamaṃ parājaye sippaṃ,",
  "anya-e1005n0:82": "Dutiyaṃ parājaye dhanaṃ;",
  "anya-e1005n0:83": "Tatiyaṃ parājaye dhammaṃ,",
  "anya-e1005n0:84": "Catutthaṃ kiṃ karissati.",
  "anya-e1005n0:85": "17.",
  "anya-e1005n0:86": "Byatta putra kimalaso,",
  "anya-e1005n0:87": "Abyatto bhārahārako;",
  "anya-e1005n0:88": "Byattako pūjito loke,",
  "anya-e1005n0:89": "Byatta putra dine dine.",
  "anya-e1005n0:90": "18.",
  "anya-e1005n0:91": "Mātā verī pitā satru,",
  "anya-e1005n0:92": "Kena bāle na sikkhitā;",
  "anya-e1005n0:93": "Sabhāmajjhe na sobhanti,",
  "anya-e1005n0:94": "Haṃsamajjhe bakoyathā.",
  "anya-e1005n0:95": "19.",
  "anya-e1005n0:96": "Kaṇṭakaṃ giri ko tikkhati,",
  "anya-e1005n0:97": "Ko añjanaṃ migakkhikaṃ;",
  "anya-e1005n0:98": "Uppathaṃ pallale ko sugandhaṃ,",
  "anya-e1005n0:99": "Kula-putta-rūpo ko pavattati;",
  "anya-e1005n0:100": "Sāmaṃ-bhāvo.",
  "anya-e1005n0:101": "20.",
  "anya-e1005n0:102": "Na rasaṃ akotambulaṃ,",
  "anya-e1005n0:103": "Adhanassa, laṅkatampi;",
  "anya-e1005n0:104": "Alonakantu byañjanaṃ,",
  "anya-e1005n0:105": "Byākaraṇaṃ asippassa.",
  "anya-e1005n0:106": "21.",
  "anya-e1005n0:107": "Sussusā sutasampanno,",
  "anya-e1005n0:108": "Sutāpaññāya pavaḍḍhati;",
  "anya-e1005n0:109": "Paññāya atthaṃ jānāti,",
  "anya-e1005n0:110": "Ñāto attho sukhāvaho.",
  "anya-e1005n0:111": "22.",
  "anya-e1005n0:112": "Bhojanaṃ methunaṃ niddā,",
  "anya-e1005n0:113": "Goṇe posepi vijjati;",
  "anya-e1005n0:114": "Vijjā viseso posassa,",
  "anya-e1005n0:115": "Taṃ hīno gosamo bhave.",
  "anya-e1005n0:116": "23.",
  "anya-e1005n0:117": "Natthi vijjāsamaṃmittaṃ,",
  "anya-e1005n0:118": "Naca byādhisamo ripu;",
  "anya-e1005n0:119": "Naca attasamaṃ pemaṃ,",
  "anya-e1005n0:120": "Naca kammasamaṃ balaṃ.",
  "anya-e1005n0:121": "24.",
  "anya-e1005n0:122": "Haṃso majjhe na kākānaṃ,",
  "anya-e1005n0:123": "Sīho gunnaṃ na sobhate;",
  "anya-e1005n0:124": "Gadrabhamajjhe turaṅgo,",
  "anya-e1005n0:125": "Bālamajjhe ca paṇḍito.",
  "anya-e1005n0:126": "25.",
  "anya-e1005n0:127": "Yāvajīvampi ce bālo,",
  "anya-e1005n0:128": "Paṇḍitaṃ payirupāsati;",
  "anya-e1005n0:129": "Na so dhammaṃ vijānāti,",
  "anya-e1005n0:130": "Dabbi sūparasaṃ yathā.",
  "anya-e1005n0:131": "26.",
  "anya-e1005n0:132": "Muhuttamapi ce viññū,",
  "anya-e1005n0:133": "Paṇḍitaṃ payirupāsati;",
  "anya-e1005n0:134": "Khippaṃ dhammaṃ vijānāti,",
  "anya-e1005n0:135": "Jivhā sūparasaṃ yathā.",
  "anya-e1005n0:136": "27.",
  "anya-e1005n0:137": "Vinā satthaṃ na gaccheyya,",
  "anya-e1005n0:138": "Sūro saṅgāmabhūmiyaṃ;",
  "anya-e1005n0:139": "Paṇḍitvāddhagū vāṇijo,",
  "anya-e1005n0:140": "Videsagamano tathā.",
  "anya-e1005n0:141": "28.",
  "anya-e1005n0:142": "Dhananāsaṃ manotāpaṃ,",
  "anya-e1005n0:143": "Ghare duccaritāni ca;",
  "anya-e1005n0:144": "Vañcanañca avamānaṃ,",
  "anya-e1005n0:145": "Paṇḍito na pakāsaye.",
  "anya-e1005n0:146": "29.",
  "anya-e1005n0:147": "Pattānurūpakaṃ vākyaṃ,",
  "anya-e1005n0:148": "Sabhāvarūpakaṃ piyaṃ;",
  "anya-e1005n0:149": "Attānurūpakaṃ kodhaṃ,",
  "anya-e1005n0:150": "Yo jānāti sa paṇḍito.",
  "anya-e1005n0:151": "30.",
  "anya-e1005n0:152": "A-dhanassa rasaṃ khādā,",
  "anya-e1005n0:153": "A-balassa hathā narā;",
  "anya-e1005n0:154": "A-paññassa vākya-kathā,",
  "anya-e1005n0:155": "Ummattaka-samā ime.",
  "anya-e1005n0:156": "31.",
  "anya-e1005n0:157": "Anavhāyaṃ gamayanto,",
  "anya-e1005n0:158": "A-pucchā bahu-bhāsako;",
  "anya-e1005n0:159": "Atta-guṇaṃ pakāsento,",
  "anya-e1005n0:160": "Ti-vidhaṃ hīna-lakkhaṇaṃ.",
  "anya-e1005n0:161": "32.",
  "anya-e1005n0:162": "Appa-rūpo bahuṃ bhāso,",
  "anya-e1005n0:163": "Appa-pañño pakāsito;",
  "anya-e1005n0:164": "Appa-pūro ghaṭo khobhe,",
  "anya-e1005n0:165": "Appa-khīrā gāvī cathe.",
  "anya-e1005n0:166": "33.",
  "anya-e1005n0:167": "Maṇḍūkepi ukre sīhe,",
  "anya-e1005n0:168": "Kākaggahe piye piye;",
  "anya-e1005n0:169": "A-paṇḍīpi paṇḍī hutvā,",
  "anya-e1005n0:170": "Dhīrā pucche vaye vaye.",
  "anya-e1005n0:171": "34.",
  "anya-e1005n0:172": "Maṇḍūkepi ukre sīhe,",
  "anya-e1005n0:173": "Sūkarepi uhe dīpe;",
  "anya-e1005n0:174": "Biḷāre sadise byagghe,",
  "anya-e1005n0:175": "Sabba dhīre sippa-same.",
  "anya-e1005n0:176": "35.",
  "anya-e1005n0:177": "Na titti rājā dhanamhi,",
  "anya-e1005n0:178": "Paṇḍitopi su-bhāsite;",
  "anya-e1005n0:179": "Cakkhuṃpi piya-dassane,",
  "anya-e1005n0:180": "Jale sāgaro na titti.",
  "anya-e1005n0:181": "36.",
  "anya-e1005n0:182": "Rūpa-yobbana-sampannā,",
  "anya-e1005n0:183": "Visātha-kutha-sambhavā;",
  "anya-e1005n0:184": "Vijjā-hīnā na sobhanti,",
  "anya-e1005n0:185": "Nigandhā iva kiṃsukā.",
  "anya-e1005n0:186": "37.",
  "anya-e1005n0:187": "Hīne putto rājāmacco,",
  "anya-e1005n0:188": "Bāla-putto ca paṇḍito;",
  "anya-e1005n0:189": "A-dhanassa dhanaṃ bahu,",
  "anya-e1005n0:190": "Purisānaṃ na maññatha.",
  "anya-e1005n0:191": "38.",
  "anya-e1005n0:192": "Yo sippa-lobhena,",
  "anya-e1005n0:193": "Bahuṃ gaṇhāti taṃ sippaṃ;",
  "anya-e1005n0:194": "Mūgova supinaṃ passaṃ,",
  "anya-e1005n0:195": "Kathetumpi na ussahe.",
  "anya-e1005n0:196": "39.",
  "anya-e1005n0:197": "Bhijjetuṃ kumbhakāro,",
  "anya-e1005n0:198": "Sobhetuṃ kumbha ghaṭṭati;",
  "anya-e1005n0:199": "Na khipituṃ apāyesu,",
  "anya-e1005n0:200": "Sissānaṃ vuḍḍhi-kāraṇā.",
  "anya-e1005n0:201": "40.",
  "anya-e1005n0:202": "Taggarañca palāsena,",
  "anya-e1005n0:203": "Yo naro upanayhati;",
  "anya-e1005n0:204": "Pattāpi surabhi vāyanti,",
  "anya-e1005n0:205": "Evaṃ dhīrūpasevanā.",
  "anya-e1005n0:206": "Paṇḍitakaṇḍo niṭṭhito.",
  "anya-e1005n0:207": "Sujanakaṇḍo",
  "anya-e1005n0:208": "41.",
  "anya-e1005n0:209": "Sabbhireva samāsetha,",
  "anya-e1005n0:210": "Sabbhi kubbetha santhavaṃ;",
  "anya-e1005n0:211": "Sataṃ saddhammamaññāya,",
  "anya-e1005n0:212": "Seyyo hoti na pāpiyo.",
  "anya-e1005n0:213": "42.",
  "anya-e1005n0:214": "Caja dujjana saṃsaggaṃ,",
  "anya-e1005n0:215": "Bhaja sādhu samāgamaṃ;",
  "anya-e1005n0:216": "Kara puññamahorattiṃ,",
  "anya-e1005n0:217": "Sara niccamaniccataṃ.",
  "anya-e1005n0:218": "43.",
  "anya-e1005n0:219": "Yathā udumbarapakkā,",
  "anya-e1005n0:220": "Bahirattakamevaca;",
  "anya-e1005n0:221": "Anto kimīhi sampuṇṇā,",
  "anya-e1005n0:222": "Evaṃ dujjanahaddayā.",
  "anya-e1005n0:223": "44.",
  "anya-e1005n0:224": "Yathāpi panasāpakkā,",
  "anya-e1005n0:225": "Bahi kaṇḍakameva ca;",
  "anya-e1005n0:226": "Anto amatasampannā,",
  "anya-e1005n0:227": "Evaṃ sujanahadayā.",
  "anya-e1005n0:228": "45.",
  "anya-e1005n0:229": "Sukkhopi candanataru na jahāti gandhaṃ,",
  "anya-e1005n0:230": "Nāgo gato naramukhe na jahāti līḷaṃ;",
  "anya-e1005n0:231": "Yantāgato madhurasaṃ na jahāti ucchu,",
  "anya-e1005n0:232": "Dukkhopi paṇḍitajano na jahāti dhammaṃ.",
  "anya-e1005n0:233": "46.",
  "anya-e1005n0:234": "Sīho nāma jighacchāpi,",
  "anya-e1005n0:235": "Paṇṇādīni na khādati;",
  "anya-e1005n0:236": "Sīho nāma kisocāpi,",
  "anya-e1005n0:237": "Nāgamaṃsaṃ na khādati.",
  "anya-e1005n0:238": "47.",
  "anya-e1005n0:239": "Kula-jāto kula-putto,",
  "anya-e1005n0:240": "Kula-vaṃsa-surakkhato;",
  "anya-e1005n0:241": "Attanā dukkha-pattopi,",
  "anya-e1005n0:242": "Hīna-kammaṃ na kāraye.",
  "anya-e1005n0:243": "48.",
  "anya-e1005n0:244": "Candanaṃ sītalaṃ loke,",
  "anya-e1005n0:245": "Tato candaṃva sītalaṃ;",
  "anya-e1005n0:246": "Canda-candanasītamhā,",
  "anya-e1005n0:247": "Sādhu vākyaṃ subhāsitaṃ.",
  "anya-e1005n0:248": "49.",
  "anya-e1005n0:249": "Udeyya bhāṇu pacchime,",
  "anya-e1005n0:250": "Merurājā nameyyapi;",
  "anya-e1005n0:251": "Sītalā narakaggipi,",
  "anya-e1005n0:252": "Pabbatagge ca uppalaṃ.",
  "anya-e1005n0:253": "Vikase na viparītaṃ,",
  "anya-e1005n0:254": "Sādhuvāyyaṃ kudācanaṃ.",
  "anya-e1005n0:255": "50.",
  "anya-e1005n0:256": "Sukhā rukkhassa chāyāva,",
  "anya-e1005n0:257": "Tato ñāti mātā pitu;",
  "anya-e1005n0:258": "Tato ācariyo rañño,",
  "anya-e1005n0:259": "Tato buddhassanekadhā.",
  "anya-e1005n0:260": "51.",
  "anya-e1005n0:261": "Bhamarā pupphamicchanti,",
  "anya-e1005n0:262": "Guṇamicchanti sujanā;",
  "anya-e1005n0:263": "Makkhikā pūtimicchanti,",
  "anya-e1005n0:264": "Dosamicchanti dujjanā.",
  "anya-e1005n0:265": "52.",
  "anya-e1005n0:266": "Mātāhīnassa dubbhāsā,",
  "anya-e1005n0:267": "Pitāhīnassa dukriyā;",
  "anya-e1005n0:268": "Ubho mātā pitā hīnā,",
  "anya-e1005n0:269": "Dubbhasāca dukīriyā.",
  "anya-e1005n0:270": "53.",
  "anya-e1005n0:271": "Mātā seṭṭhassa subhāsā,",
  "anya-e1005n0:272": "Pitā seṭṭhassa sukriyā;",
  "anya-e1005n0:273": "Ubho mātā pitā seṭṭhā,",
  "anya-e1005n0:274": "Subhāsāca sukīriyā.",
  "anya-e1005n0:275": "54.",
  "anya-e1005n0:276": "Saṅgāme sūra-micchanti,",
  "anya-e1005n0:277": "Mantīsu akūtūhalaṃ;",
  "anya-e1005n0:278": "Piyañca annapānesu,",
  "anya-e1005n0:279": "Atthe jāte ca paṇḍitaṃ.",
  "anya-e1005n0:280": "55.",
  "anya-e1005n0:281": "Sunakho sunakhaṃ disvā,",
  "anya-e1005n0:282": "Dantaṃ dasseti hiṃsituṃ;",
  "anya-e1005n0:283": "Dujjano sujanaṃ disvā,",
  "anya-e1005n0:284": "Rosayaṃ hiṃsamicchati.",
  "anya-e1005n0:285": "56.",
  "anya-e1005n0:286": "Mā ca vegena kiccāni,",
  "anya-e1005n0:287": "Karosi kārāpesi vā;",
  "anya-e1005n0:288": "Sahasā kāritaṃ kammaṃ,",
  "anya-e1005n0:289": "Mando pacchānutappati.",
  "anya-e1005n0:290": "57.",
  "anya-e1005n0:291": "Kodhaṃ vihitvā na kadācinasoce,",
  "anya-e1005n0:292": "Makkhappahānaṃ isayo avaṇṇayuṃ;",
  "anya-e1005n0:293": "Sabbesa phārusa-vacaṃ khametha,",
  "anya-e1005n0:294": "Etaṃ khantiṃ uttamamāhu santo.",
  "anya-e1005n0:295": "58.",
  "anya-e1005n0:296": "Dukkho nivāso sambādhe,",
  "anya-e1005n0:297": "Ṭhāne asucisaṅkate;",
  "anya-e1005n0:298": "Tato arimhi appiye,",
  "anya-e1005n0:299": "Tatopi akataññunā.",
  "anya-e1005n0:300": "59.",
  "anya-e1005n0:301": "Ovādeyyānusāseyya,",
  "anya-e1005n0:302": "Gāpakā ca nivāraye;",
  "anya-e1005n0:303": "Satañhi so piyo hoti,",
  "anya-e1005n0:304": "Asataṃ hoti appiyo.",
  "anya-e1005n0:305": "60.",
  "anya-e1005n0:306": "Uttamattanivātena,",
  "anya-e1005n0:307": "Sūraṃ bhedena nijjaye;",
  "anya-e1005n0:308": "Nīcaṃ appaka dānena,",
  "anya-e1005n0:309": "Vīriyena samaṃ jaye.",
  "anya-e1005n0:310": "61.",
  "anya-e1005n0:311": "Na visaṃ visamiccāhu,",
  "anya-e1005n0:312": "Dhanaṃ saṅghassa uccate;",
  "anya-e1005n0:313": "Visaṃ ekaṃva hanati,",
  "anya-e1005n0:314": "Sabbaṃ saṅghassa santakaṃ.",
  "anya-e1005n0:315": "62.",
  "anya-e1005n0:316": "Javane bhadraṃ jānanti,",
  "anya-e1005n0:317": "Baliddañca vāhenā;",
  "anya-e1005n0:318": "Duhena dhenuṃ jānanti,",
  "anya-e1005n0:319": "Bhāsamānena paṇḍitaṃ.",
  "anya-e1005n0:320": "63.",
  "anya-e1005n0:321": "Dhanamappampi sādhūnaṃ,",
  "anya-e1005n0:322": "Kūpe vāriva nissayo;",
  "anya-e1005n0:323": "Bahuṃ api asādhūnaṃ,",
  "anya-e1005n0:324": "Naca vārīva aṇṇave.",
  "anya-e1005n0:325": "64.",
  "anya-e1005n0:326": "Najjo pivanti no āpaṃ,",
  "anya-e1005n0:327": "Rukkhā khādanti no phalaṃ;",
  "anya-e1005n0:328": "Vassanti kvaci no meghā,",
  "anya-e1005n0:329": "Paratthāya sataṃ dhanaṃ.",
  "anya-e1005n0:330": "65.",
  "anya-e1005n0:331": "Apattheyyaṃ na pattheyya,",
  "anya-e1005n0:332": "A cinteyyaṃ na cintaye;",
  "anya-e1005n0:333": "Dhammameva sucinteyya,",
  "anya-e1005n0:334": "Kālaṃ moghaṃ na accaye.",
  "anya-e1005n0:335": "66.",
  "anya-e1005n0:336": "Acintitampi bhavati,",
  "anya-e1005n0:337": "Cintitampi vinassati;",
  "anya-e1005n0:338": "Na hi cintāmayā bhogā,",
  "anya-e1005n0:339": "Itthiyā purisassavā.",
  "anya-e1005n0:340": "67.",
  "anya-e1005n0:341": "Asantassa piyo hoti,",
  "anya-e1005n0:342": "Sante na kurute piyaṃ;",
  "anya-e1005n0:343": "Asataṃ dhammaṃ roceti,",
  "anya-e1005n0:344": "Taṃ parābhavato mukhaṃ.",
  "anya-e1005n0:345": "Sujanakaṇḍo niṭṭhito.",
  "anya-e1005n0:346": "4. Bāladujjanakaṇḍo",
  "anya-e1005n0:347": "68.",
  "anya-e1005n0:348": "Atippiyo na kātabbo,",
  "anya-e1005n0:349": "Khalo kotūhalaṃ karo;",
  "anya-e1005n0:350": "Sirasā vahyamānopi,",
  "anya-e1005n0:351": "Aḍḍhapūro ghaṭo yathā.",
  "anya-e1005n0:352": "69.",
  "anya-e1005n0:353": "Sappo duṭṭho khalo duṭṭho,",
  "anya-e1005n0:354": "Sappo duṭṭhataro khalo;",
  "anya-e1005n0:355": "Mantosadhehi so sappo,",
  "anya-e1005n0:356": "Khalo kenupasammati.",
  "anya-e1005n0:357": "70.",
  "anya-e1005n0:358": "Yo bālo maññati bālyaṃ,",
  "anya-e1005n0:359": "Paṇḍito vāpi tena so;",
  "anya-e1005n0:360": "Bālova paṇḍitamānī,",
  "anya-e1005n0:361": "So ve bāloti vuccati.",
  "anya-e1005n0:362": "71.",
  "anya-e1005n0:363": "Madhuṃva maññatī bālo,",
  "anya-e1005n0:364": "Yāva pāpaṃ na paccati;",
  "anya-e1005n0:365": "Yadāca paccatī pāpaṃ,",
  "anya-e1005n0:366": "Atha dukkhaṃ nigacchati.",
  "anya-e1005n0:367": "72.",
  "anya-e1005n0:368": "Na sādhu balavā bālo,",
  "anya-e1005n0:369": "Sahasā vindate dhanaṃ;",
  "anya-e1005n0:370": "Kāyassa bhedā duppañño,",
  "anya-e1005n0:371": "Nirayaṃ sopapajjati.",
  "anya-e1005n0:372": "73.",
  "anya-e1005n0:373": "Ghare duṭṭho ca mūsīko ca,",
  "anya-e1005n0:374": "Vane duṭṭho ca vānaro;",
  "anya-e1005n0:375": "Sakuṇe ca duṭṭho kāko,",
  "anya-e1005n0:376": "Nare duṭṭhoca brāhmaṇo.",
  "anya-e1005n0:377": "74.",
  "anya-e1005n0:378": "Dīghā jāgarato ratti,",
  "anya-e1005n0:379": "Dīghaṃ santassa yojanaṃ;",
  "anya-e1005n0:380": "Dīgho bālāna saṃsāro,",
  "anya-e1005n0:381": "Saddhammaṃ a-vijānataṃ.",
  "anya-e1005n0:382": "75.",
  "anya-e1005n0:383": "Tila mattaṃ paresaṃva,",
  "anya-e1005n0:384": "Appa dosañca passati;",
  "anya-e1005n0:385": "Nāḷikerampi sadosaṃ,",
  "anya-e1005n0:386": "Khala-jāto na passati.",
  "anya-e1005n0:387": "76.",
  "anya-e1005n0:388": "Nattadosaṃ pare jaññā,",
  "anya-e1005n0:389": "Jaññā dosaṃ parassatu;",
  "anya-e1005n0:390": "Guyhe kummo aṅgāni,",
  "anya-e1005n0:391": "Para dosañca lakkhaye.",
  "anya-e1005n0:392": "77.",
  "anya-e1005n0:393": "Paṇḍitassa pasaṃsāya,",
  "anya-e1005n0:394": "Daṇḍo bālena dīyate;",
  "anya-e1005n0:395": "Paṇḍito paṇḍiteneva,",
  "anya-e1005n0:396": "Vaṇṇitova suvaṇṇito.",
  "anya-e1005n0:397": "78.",
  "anya-e1005n0:398": "Luddhaṃ atthena gaṇheyya,",
  "anya-e1005n0:399": "Thaddhaṃ añjali kammunā;",
  "anya-e1005n0:400": "Chandānuvattiyā mūḷhaṃ,",
  "anya-e1005n0:401": "Yathābhūtena paṇḍitaṃ.",
  "anya-e1005n0:402": "Bāladujjanakaṇḍo niṭṭhito.",
  "anya-e1005n0:403": "4. Mittakaṇḍo",
  "anya-e1005n0:404": "79.",
  "anya-e1005n0:405": "Hitakāro paro bandhu,",
  "anya-e1005n0:406": "Bandhūpi ahito paro;",
  "anya-e1005n0:407": "A hito dehajo byādhi,",
  "anya-e1005n0:408": "Hitaṃ araññamosadhaṃ.",
  "anya-e1005n0:409": "80.",
  "anya-e1005n0:410": "Parokkhe guṇa-hantāraṃ,",
  "anya-e1005n0:411": "Paccakkhe piyavādinaṃ;",
  "anya-e1005n0:412": "Vajjeyya tādisaṃ mittaṃ,",
  "anya-e1005n0:413": "Visakumbhe yathā madhuṃ.",
  "anya-e1005n0:414": "81.",
  "anya-e1005n0:415": "Dhanahīne caje mitto,",
  "anya-e1005n0:416": "Puttadārā sahodarā;",
  "anya-e1005n0:417": "Dhanavantaṃva sevanti,",
  "anya-e1005n0:418": "Dhanaṃ loke mahā sakhā.",
  "anya-e1005n0:419": "82.",
  "anya-e1005n0:420": "Jāneyya pesena bhaccaṃ,",
  "anya-e1005n0:421": "Bandhuṃ vāpi bhayāgate;",
  "anya-e1005n0:422": "Appakāsu tathā mittaṃ,",
  "anya-e1005n0:423": "Dārañca vibhavakkhaye.",
  "anya-e1005n0:424": "83.",
  "anya-e1005n0:425": "So bandhu yo hite yutto,",
  "anya-e1005n0:426": "Pitaro honti posako;",
  "anya-e1005n0:427": "Taṃ mittaṃ yattha vissāso,",
  "anya-e1005n0:428": "Sā bhariyā ca yassa nibbūti.",
  "anya-e1005n0:429": "84.",
  "anya-e1005n0:430": "Na vissase a-vissattaṃ,",
  "anya-e1005n0:431": "Mittañcāpi na vissase;",
  "anya-e1005n0:432": "Kadāci kupito mito,",
  "anya-e1005n0:433": "Sabbaṃ dosaṃ pakāsaye.",
  "anya-e1005n0:434": "85.",
  "anya-e1005n0:435": "Sakiṃ duṭṭhañca yo mittaṃ,",
  "anya-e1005n0:436": "Puna sandhitumicchati;",
  "anya-e1005n0:437": "So maccuṃ upagaṇhāti,",
  "anya-e1005n0:438": "Gabbhamassatarī yathā.",
  "anya-e1005n0:439": "86.",
  "anya-e1005n0:440": "Vahe a-mittaṃ khandhena,",
  "anya-e1005n0:441": "Yāva kālo anāgato;",
  "anya-e1005n0:442": "Tameva āgate kāle,",
  "anya-e1005n0:443": "Sele bhinde ghaṭaṃ iva.",
  "anya-e1005n0:444": "87.",
  "anya-e1005n0:445": "Iṇaseso aggiseso,",
  "anya-e1005n0:446": "Satruseso tatheva ca;",
  "anya-e1005n0:447": "Punappunaṃ vivaḍḍhanti,",
  "anya-e1005n0:448": "Tasmā sesaṃ na kāraye.",
  "anya-e1005n0:449": "88.",
  "anya-e1005n0:450": "Padumaṃva mukhaṃ yassa,",
  "anya-e1005n0:451": "Vācā candana sītalā;",
  "anya-e1005n0:452": "Tādisaṃ nopaseveyya,",
  "anya-e1005n0:453": "Hadayetu halāhalaṃ.",
  "anya-e1005n0:454": "89.",
  "anya-e1005n0:455": "Na seve pharusaṃ sāmiṃ,",
  "anya-e1005n0:456": "Naca seveyya macchariṃ;",
  "anya-e1005n0:457": "Tato apaggaṇhaṃ sāmiṃ,",
  "anya-e1005n0:458": "Neva niggahitaṃ tato.",
  "anya-e1005n0:459": "90.",
  "anya-e1005n0:460": "Siṅgī paññāsa hatthena,",
  "anya-e1005n0:461": "Vajje satena vājinaṃ;",
  "anya-e1005n0:462": "Hatthiṃ dantiṃ sahassena,",
  "anya-e1005n0:463": "Desa cāgena dujjanaṃ.",
  "anya-e1005n0:464": "91.",
  "anya-e1005n0:465": "Kudesañca kumittañca,",
  "anya-e1005n0:466": "Kukulañca kubandhavaṃ;",
  "anya-e1005n0:467": "Kudārañca kudāsañca,",
  "anya-e1005n0:468": "Dūrato parivajjaye.",
  "anya-e1005n0:469": "92.",
  "anya-e1005n0:470": "Rogāture ca dubbhikkhe,",
  "anya-e1005n0:471": "Byasane sattu viggahe;",
  "anya-e1005n0:472": "Rājadvāre susāne ca,",
  "anya-e1005n0:473": "Ye tiṭṭhanti sumittakā.",
  "anya-e1005n0:474": "93.",
  "anya-e1005n0:475": "Sītavāco bahumitto,",
  "anya-e1005n0:476": "Pharuso appamittako;",
  "anya-e1005n0:477": "Upamaṃ ettha ñātabbā,",
  "anya-e1005n0:478": "Canda-sūriya-rājūnaṃ.",
  "anya-e1005n0:479": "Mittakaṇḍo niṭṭhito.",
  "anya-e1005n0:480": "5. Itthikaṇḍo",
  "anya-e1005n0:481": "94.",
  "anya-e1005n0:482": "Kokilānaṃ saddaṃ rūpaṃ,",
  "anya-e1005n0:483": "Nārīrūpaṃ patibbatā;",
  "anya-e1005n0:484": "Vijjā rūpaṃ a-rūpānaṃ,",
  "anya-e1005n0:485": "Khamā rūpaṃ tapassinaṃ.",
  "anya-e1005n0:486": "95.",
  "anya-e1005n0:487": "Itthīnañca dhanaṃ rūpaṃ,",
  "anya-e1005n0:488": "Purisānaṃ vijjā dhanaṃ;",
  "anya-e1005n0:489": "Bhikkhūnañca dhanaṃ sīlaṃ,",
  "anya-e1005n0:490": "Rājānañca dhanaṃ balaṃ.",
  "anya-e1005n0:491": "96.",
  "anya-e1005n0:492": "Tapassino kisā sobhā,",
  "anya-e1005n0:493": "Thūlā sobhā catuppadā;",
  "anya-e1005n0:494": "Purisā vijjavā sobhā,",
  "anya-e1005n0:495": "Itthī sobhāsa sāmikā.",
  "anya-e1005n0:496": "97.",
  "anya-e1005n0:497": "Pañca ratyā sugandhabbā,",
  "anya-e1005n0:498": "Satta ratyā dhanuggahā;",
  "anya-e1005n0:499": "Eka māsā subhariyā,",
  "anya-e1005n0:500": "Aḍḍha māsā sissā malā.",
  "anya-e1005n0:501": "98.",
  "anya-e1005n0:502": "Hiṃ ramati paṅa,",
  "anya-e1005n0:503": "Haṅa ramati poka.",
  "anya-e1005n0:504": "Thī ramati pu,",
  "anya-e1005n0:505": "Khu ramati dhaṃ.",
  "anya-e1005n0:506": "99.",
  "anya-e1005n0:507": "Jiṇṇamannaṃ pasaṃseyya,",
  "anya-e1005n0:508": "Dārañca gatayobbanaṃ;",
  "anya-e1005n0:509": "Raṇā punāgatā sūraṃ,",
  "anya-e1005n0:510": "Sassañca gehamāgataṃ.",
  "anya-e1005n0:511": "100.",
  "anya-e1005n0:512": "Dvatti-patikā nārī ca,",
  "anya-e1005n0:513": "Bhikkhu dvatti-vihāriko;",
  "anya-e1005n0:514": "Dvatti-pāsa-mutto pakkhī,",
  "anya-e1005n0:515": "Kata-māyā bahūtharaṃ.",
  "anya-e1005n0:516": "101.",
  "anya-e1005n0:517": "Dujjanaṃ pahārādame,",
  "anya-e1005n0:518": "Mittaṃ dame a-bhāṇikā;",
  "anya-e1005n0:519": "Itthiñca byasanā dame,",
  "anya-e1005n0:520": "Rāginaṃ appa bhojanā.",
  "anya-e1005n0:521": "102.",
  "anya-e1005n0:522": "Na ratti vinā candimā,",
  "anya-e1005n0:523": "Vīciṃ vinā ca aṇṇavo;",
  "anya-e1005n0:524": "Haṃsaṃ vinā pokkharaṇī,",
  "anya-e1005n0:525": "Patiṃ kaññāca sobhate.",
  "anya-e1005n0:526": "103.",
  "anya-e1005n0:527": "Patinā janito bhogo,",
  "anya-e1005n0:528": "Itthiyāva saṃgoppito;",
  "anya-e1005n0:529": "Purisova hi padhāno,",
  "anya-e1005n0:530": "Itthī suttaṃva sūciyā.",
  "anya-e1005n0:531": "104.",
  "anya-e1005n0:532": "Sabbānadī vaṅkanadī,",
  "anya-e1005n0:533": "Sabbe kaṭṭhamayā vanā;",
  "anya-e1005n0:534": "Sabbitthiyo kare pāpaṃ,",
  "anya-e1005n0:535": "Labhamāne nivātake.",
  "anya-e1005n0:536": "105.",
  "anya-e1005n0:537": "Vivādasīliṃ usūyabhāṇiniṃ,",
  "anya-e1005n0:538": "Sampassataṇhiṃ bahupākabhuttiniṃ;",
  "anya-e1005n0:539": "Aggantabhuttiṃ paragehavāsiniṃ,",
  "anya-e1005n0:540": "Nāriṃ caje puttasatampi pūmā.",
  "anya-e1005n0:541": "106.",
  "anya-e1005n0:542": "Bhuttesu maṇḍesu janīva kantinī,",
  "anya-e1005n0:543": "Guyheca ṭhāne bhaginīva hiriṇī;",
  "anya-e1005n0:544": "Kammesu pattesu karoti dāsīva,",
  "anya-e1005n0:545": "Bhayesu mantī sayanesu rāmaye;",
  "anya-e1005n0:546": "Rūpīsu sikkhī kupanesu khantinī,",
  "anya-e1005n0:547": "Sā nārī seṭṭhāti vadanti paṇḍitā;",
  "anya-e1005n0:548": "Kāyassa bhedāca divebhaveyya sā.",
  "anya-e1005n0:549": "107.",
  "anya-e1005n0:550": "Sāmā migakkhī tanumajjhagattā,",
  "anya-e1005n0:551": "Sūrū sukesī samadantapantī;",
  "anya-e1005n0:552": "Gambhīranābhī yuvatī susīlī,",
  "anya-e1005n0:553": "Hīne kule jātāpi vivāhyā.",
  "anya-e1005n0:554": "108.",
  "anya-e1005n0:555": "Saradaṃratu-kālānaṃ,",
  "anya-e1005n0:556": "Bhariyānaṃ rūpavatī;",
  "anya-e1005n0:557": "Jeṭṭho padhānaṃ puttānaṃ,",
  "anya-e1005n0:558": "Disānaṃ uttarādisā.",
  "anya-e1005n0:559": "109.",
  "anya-e1005n0:560": "Yā icche puriso hotuṃ,",
  "anya-e1005n0:561": "Jāti jāti punappunaṃ;",
  "anya-e1005n0:562": "Sāmikaṃ apacāyeyya,",
  "anya-e1005n0:563": "Indaṃva pāricārikā.",
  "anya-e1005n0:564": "110.",
  "anya-e1005n0:565": "Yo icche puriso hotuṃ,",
  "anya-e1005n0:566": "Jāti jāti punappunaṃ;",
  "anya-e1005n0:567": "Paradānaṃ vivajjeyya,",
  "anya-e1005n0:568": "Dhotapādova kaddamaṃ.",
  "anya-e1005n0:569": "111.",
  "anya-e1005n0:570": "Atikkanta vayo poso,",
  "anya-e1005n0:571": "Āneti timbaruttaniṃ;",
  "anya-e1005n0:572": "Tassā issā asaddhāti,",
  "anya-e1005n0:573": "Taṃ parābhavato mukhaṃ.",
  "anya-e1005n0:574": "Itthikaṇḍo niṭṭhito.",
  "anya-e1005n0:575": "6. Rājakaṇḍo",
  "anya-e1005n0:576": "112.",
  "anya-e1005n0:577": "Ekayāmaṃ saye rājā,",
  "anya-e1005n0:578": "Dviyāmaññeva paṇḍito;",
  "anya-e1005n0:579": "Gharāvāso tiyāmaṃva,",
  "anya-e1005n0:580": "Catuyāmaṃ tu yācako.",
  "anya-e1005n0:581": "113.",
  "anya-e1005n0:582": "Dhanavā sutavā rājā,",
  "anya-e1005n0:583": "Nadī vejjo cimepañca;",
  "anya-e1005n0:584": "Yattha dese na vijjanti,",
  "anya-e1005n0:585": "Na tattha divasaṃ vase.",
  "anya-e1005n0:586": "114.",
  "anya-e1005n0:587": "Yasmiṃ padese na māno,",
  "anya-e1005n0:588": "Na pemaṃ naca bandhavā;",
  "anya-e1005n0:589": "Naca vijjāgamo koci,",
  "anya-e1005n0:590": "Na tattha divasaṃ vase.",
  "anya-e1005n0:591": "115.",
  "anya-e1005n0:592": "Aputtakaṃ gharaṃ suññaṃ,",
  "anya-e1005n0:593": "Raṭṭhaṃ suññaṃ arājakaṃ;",
  "anya-e1005n0:594": "A sippassa mukhaṃ suññaṃ,",
  "anya-e1005n0:595": "Sabba suññaṃ daliddakā.",
  "anya-e1005n0:596": "116.",
  "anya-e1005n0:597": "Dhanamiccheyya vāṇijjo,",
  "anya-e1005n0:598": "Vijjamicche bhajesutaṃ;",
  "anya-e1005n0:599": "Puttamicche taruṇitthiṃ,",
  "anya-e1005n0:600": "Rājāmaccaṃ vasaṃ game.",
  "anya-e1005n0:601": "117.",
  "anya-e1005n0:602": "Naṭṭhoyati asantuṭṭho,",
  "anya-e1005n0:603": "Santuṭṭho ca mahīpati;",
  "anya-e1005n0:604": "Lajjā ca gaṇikā naṭṭhā,",
  "anya-e1005n0:605": "Nillajjā kuladhītikā.",
  "anya-e1005n0:606": "118.",
  "anya-e1005n0:607": "Pakkhīnaṃ balamākāso,",
  "anya-e1005n0:608": "Macchānamudakaṃ balaṃ;",
  "anya-e1005n0:609": "Dubbalassa balaṃ rājā,",
  "anya-e1005n0:610": "Kumārānaṃ rudaṃ balaṃ.",
  "anya-e1005n0:611": "119.",
  "anya-e1005n0:612": "Khamā jāgariyuṭṭhānaṃ,",
  "anya-e1005n0:613": "Saṃvibhāgo dayikkhaṇā;",
  "anya-e1005n0:614": "Nāyakassa guṇā ete,",
  "anya-e1005n0:615": "Icchitabbā sataṃ guṇā.",
  "anya-e1005n0:616": "120.",
  "anya-e1005n0:617": "Sakiṃ vadanti rājāno,",
  "anya-e1005n0:618": "Sakiṃ samaṇabrāhmaṇā;",
  "anya-e1005n0:619": "Sakiṃ sappurisā loke,",
  "anya-e1005n0:620": "Esa dhammo sanantano.",
  "anya-e1005n0:621": "121.",
  "anya-e1005n0:622": "Alaso gihī kāmabhogī na sādhu,",
  "anya-e1005n0:623": "Asaññato pabbajito na sādhu;",
  "anya-e1005n0:624": "Rājā anisammakārī na sādhu,",
  "anya-e1005n0:625": "Paṇḍito kodhano taṃpi na sādhu.",
  "anya-e1005n0:626": "122.",
  "anya-e1005n0:627": "Bahavo yattha nettāro,",
  "anya-e1005n0:628": "Sabbe paṇḍitamānino;",
  "anya-e1005n0:629": "Sabbe mahattamicchanti,",
  "anya-e1005n0:630": "Tesaṃ kammaṃ vinassati.",
  "anya-e1005n0:631": "123.",
  "anya-e1005n0:632": "Āyaṃ khayaṃ sayaṃ jaññā,",
  "anya-e1005n0:633": "Rājā sayaṃ katākataṃ;",
  "anya-e1005n0:634": "Niggahe niggahetabbaṃ,",
  "anya-e1005n0:635": "Paggahe paggahārahaṃ.",
  "anya-e1005n0:636": "124.",
  "anya-e1005n0:637": "Piṭṭhitokkaṃ niseveyya,",
  "anya-e1005n0:638": "Kucchināva hutāsanaṃ;",
  "anya-e1005n0:639": "Sāmikaṃ sabbabhāgena,",
  "anya-e1005n0:640": "Paralokaṃ amohavā.",
  "anya-e1005n0:641": "125.",
  "anya-e1005n0:642": "Aggi āpo itthimūḷho,",
  "anya-e1005n0:643": "Sappo rāja-kulānica;",
  "anya-e1005n0:644": "Apayantena gantabbā,",
  "anya-e1005n0:645": "Acceka-pāṇahārakā.",
  "anya-e1005n0:646": "126.",
  "anya-e1005n0:647": "Paduṭṭha-bhariya-saṃvāso,",
  "anya-e1005n0:648": "Paduṭṭha citta dāsako;",
  "anya-e1005n0:649": "Sa-sappe ca ghare vāso,",
  "anya-e1005n0:650": "Maccu eva na saṃsayo.",
  "anya-e1005n0:651": "127.",
  "anya-e1005n0:652": "Mūḷha sisso padesena,",
  "anya-e1005n0:653": "Kunārī bharaṇena ca;",
  "anya-e1005n0:654": "Asatā sampayogena,",
  "anya-e1005n0:655": "Paṇḍitoppavasīdati.",
  "anya-e1005n0:656": "128.",
  "anya-e1005n0:657": "Mātā puttakaraṃ pāpaṃ,",
  "anya-e1005n0:658": "Sissapāpaṃ gurukatā;",
  "anya-e1005n0:659": "Rājā raṭṭhakaraṃ pāpaṃ,",
  "anya-e1005n0:660": "Rājapāpaṃ purohito.",
  "anya-e1005n0:661": "129.",
  "anya-e1005n0:662": "Akodhena jine kodhaṃ,",
  "anya-e1005n0:663": "Asādhuṃ sādhunā jine;",
  "anya-e1005n0:664": "Jine macchariṃ dānena,",
  "anya-e1005n0:665": "Saccenālīkavādinaṃ;",
  "anya-e1005n0:666": "130.",
  "anya-e1005n0:667": "Adantaṃ damanaṃ dānaṃ,",
  "anya-e1005n0:668": "Dānaṃ sabbattha sādhakaṃ;",
  "anya-e1005n0:669": "Dānena piya vācāya,",
  "anya-e1005n0:670": "Unnamanti namanti ca;",
  "anya-e1005n0:671": "131.",
  "anya-e1005n0:672": "Dānaṃ sinehabhesajjaṃ,",
  "anya-e1005n0:673": "Maccheraṃ dosanosadhaṃ;",
  "anya-e1005n0:674": "Dānaṃ yasassī bhesajjaṃ,",
  "anya-e1005n0:675": "Maccheraṃ kapaṇosadhaṃ.",
  "anya-e1005n0:676": "132.",
  "anya-e1005n0:677": "Bahūnamappasārānaṃ,",
  "anya-e1005n0:678": "Sāmaggiyā jayaṃ jaye;",
  "anya-e1005n0:679": "Tiṇehi vattate yottaṃ,",
  "anya-e1005n0:680": "Tena nāgopi bajjhate.",
  "anya-e1005n0:681": "133.",
  "anya-e1005n0:682": "Sahāyo asamatthopi,",
  "anya-e1005n0:683": "Tejasā kiṃkarissati;",
  "anya-e1005n0:684": "Nivāte jalito aggi,",
  "anya-e1005n0:685": "Sayame vūpasampati.",
  "anya-e1005n0:686": "134.",
  "anya-e1005n0:687": "Na raññā samakaṃ bhuñje,",
  "anya-e1005n0:688": "Kāmabhogaṃ kudācanaṃ;",
  "anya-e1005n0:689": "Ākappaṃ rasa bhuttiṃvā,",
  "anya-e1005n0:690": "Mālā gandha vilepanaṃ;",
  "anya-e1005n0:691": "Vatthaṃ sabbaalaṅkāraṃ,",
  "anya-e1005n0:692": "Na raññā sadisaṃ kare.",
  "anya-e1005n0:693": "135.",
  "anya-e1005n0:694": "Na me rājā sakhā hoti,",
  "anya-e1005n0:695": "Na rājā hoti methuno;",
  "anya-e1005n0:696": "Eso sāmiko mayhanti,",
  "anya-e1005n0:697": "Citte niṭṭhaṃ suthāpaye.",
  "anya-e1005n0:698": "136.",
  "anya-e1005n0:699": "Nātidūre bhaje rañño,",
  "anya-e1005n0:700": "Nāccāsannopavātake;",
  "anya-e1005n0:701": "Ujuke nātininne ca,",
  "anya-e1005n0:702": "Na bhaje uccamāsane.",
  "anya-e1005n0:703": "Chadose vajje sevako,",
  "anya-e1005n0:704": "Tiṭṭhe aggiṃva saṃyato.",
  "anya-e1005n0:705": "137.",
  "anya-e1005n0:706": "Guṇī sabbaññu tulyopi,",
  "anya-e1005n0:707": "Nasobhati anissayo;",
  "anya-e1005n0:708": "Anagghamopi maṇiseṭṭho,",
  "anya-e1005n0:709": "Hemaṃ nissāya sobhati.",
  "anya-e1005n0:710": "Rājakaṇḍo niṭṭhito.",
  "anya-e1005n0:711": "7. Pakiṇṇakakaṇḍo",
  "anya-e1005n0:712": "138.",
  "anya-e1005n0:713": "Itthimisse kutosīlaṃ,",
  "anya-e1005n0:714": "Maṃsa bhakkhe kutodayā;",
  "anya-e1005n0:715": "Surā pāne kutosaccaṃ,",
  "anya-e1005n0:716": "Mahālobhe kutohirī;",
  "anya-e1005n0:717": "Mahātande kutosippaṃ,",
  "anya-e1005n0:718": "Mahā kodhe kutodhanaṃ.",
  "anya-e1005n0:719": "139.",
  "anya-e1005n0:720": "Surā yogo vikālo ca,",
  "anya-e1005n0:721": "Samajja caraṇālasaṃ;",
  "anya-e1005n0:722": "Khiḍḍādhutto pāpamitto,",
  "anya-e1005n0:723": "Bhoganāsamukhā ime.",
  "anya-e1005n0:724": "140.",
  "anya-e1005n0:725": "Divā nādikkhā vattabbaṃ,",
  "anya-e1005n0:726": "Ratto nāvacanena ca;",
  "anya-e1005n0:727": "Sañcareyya bhayā bhīto,",
  "anya-e1005n0:728": "Vane vanacarī yathā.",
  "anya-e1005n0:729": "141.",
  "anya-e1005n0:730": "Jīvantāpi matāpañca,",
  "anya-e1005n0:731": "Byāsena parikittitā;",
  "anya-e1005n0:732": "Dukkhito byādhitomūḷho,",
  "anya-e1005n0:733": "Iṇavā nityasevako.",
  "anya-e1005n0:734": "142.",
  "anya-e1005n0:735": "Anāgataṃ bhayaṃ disvā,",
  "anya-e1005n0:736": "Dūrato parivajjaye;",
  "anya-e1005n0:737": "Āgatañca bhayaṃ disvā,",
  "anya-e1005n0:738": "A bhīto hoti paṇḍito.",
  "anya-e1005n0:739": "143.",
  "anya-e1005n0:740": "Niddāluko pamattoca,",
  "anya-e1005n0:741": "Sukhatto rogavālaso;",
  "anya-e1005n0:742": "Mahiccho kammārāmoca,",
  "anya-e1005n0:743": "Satte te satthavajjitā.",
  "anya-e1005n0:744": "144.",
  "anya-e1005n0:745": "Duggataṃ gaccha he lābha,",
  "anya-e1005n0:746": "Lābhī lābhena pūrati;",
  "anya-e1005n0:747": "Thale pavassa pajjunna,",
  "anya-e1005n0:748": "Sindhu āpena pūrati;",
  "anya-e1005n0:749": "Natthidaṃ kammappadhānakaṃ.",
  "anya-e1005n0:750": "145.",
  "anya-e1005n0:751": "Na hi koci kate kicce,",
  "anya-e1005n0:752": "Kattāraṃ samupekkhate;",
  "anya-e1005n0:753": "Tasmā sabbāni kiccāni,",
  "anya-e1005n0:754": "Sāva sesena kāraye.",
  "anya-e1005n0:755": "146.",
  "anya-e1005n0:756": "Tūlaṃ sallahukaṃ loke,",
  "anya-e1005n0:757": "Tato cāpalla-jātiko;",
  "anya-e1005n0:758": "Tato vuḍḍha manovādo,",
  "anya-e1005n0:759": "Pamatto buddhasāsane.",
  "anya-e1005n0:760": "147.",
  "anya-e1005n0:761": "Pāsāṇachattaṃ garukaṃ,",
  "anya-e1005n0:762": "Tato devānacikkhaṇaṃ;",
  "anya-e1005n0:763": "Tato vuḍḍhānamovādo,",
  "anya-e1005n0:764": "Tato buddhassa sāsanaṃ.",
  "anya-e1005n0:765": "148.",
  "anya-e1005n0:766": "Kāyassa dakkhiṇa hattho,",
  "anya-e1005n0:767": "Doso ettha kaniṭṭhako;",
  "anya-e1005n0:768": "Kaṇṇa ghānāna-makkhīnaṃ,",
  "anya-e1005n0:769": "Vāmo tu pāda-pāsako.",
  "anya-e1005n0:770": "149.",
  "anya-e1005n0:771": "Tambūlassa majjha patte,",
  "anya-e1005n0:772": "Kuvero rakkhatī sadā;",
  "anya-e1005n0:773": "Mūlamhi rakkhati yakkho,",
  "anya-e1005n0:774": "Aggamhi kālakaṇṇikā;",
  "anya-e1005n0:775": "Tāni bhuñjeyya chinditvā,",
  "anya-e1005n0:776": "Sirī evaṃ pavaḍḍhati.",
  "anya-e1005n0:777": "150.",
  "anya-e1005n0:778": "Sampuṇṇarakkho brahmāva,",
  "anya-e1005n0:779": "Accurakkho ca bissaṇo;",
  "anya-e1005n0:780": "Tasmā hi te pūjayantu,",
  "anya-e1005n0:781": "Sadā mānenti taṃ naraṃ.",
  "anya-e1005n0:782": "151.",
  "anya-e1005n0:783": "Goṇā hi sabbagihīnaṃ,",
  "anya-e1005n0:784": "Posakā bhogadāyakā;",
  "anya-e1005n0:785": "Tasmā hi mātā pitūva,",
  "anya-e1005n0:786": "Mānaye sakkareyya ca.",
  "anya-e1005n0:787": "152.",
  "anya-e1005n0:788": "Yeca khādanti gomaṃsaṃ,",
  "anya-e1005n0:789": "Mātu maṃsaṃva khādare;",
  "anya-e1005n0:790": "Matesu tesu gijjhānaṃ,",
  "anya-e1005n0:791": "Dade sote ca vāhaye.",
  "anya-e1005n0:792": "153.",
  "anya-e1005n0:793": "Gurusiddho sippārambho,",
  "anya-e1005n0:794": "Ravi sokrā ca majjhimo;",
  "anya-e1005n0:795": "Na sippo buddhacandaro,",
  "anya-e1005n0:796": "Sorī aṅgāca maraṇaṃ.",
  "anya-e1005n0:797": "154.",
  "anya-e1005n0:798": "Aṭṭhamiyaṃ guruṃ hanti,",
  "anya-e1005n0:799": "Sissaṃ hanti catuddasiṃ;",
  "anya-e1005n0:800": "Sippaṃ hanti dasa sippaṃ,",
  "anya-e1005n0:801": "Mātāpitā ca puṇṇamiṃ.",
  "anya-e1005n0:802": "155.",
  "anya-e1005n0:803": "Nāḷikaṃ satta nabhuñje,",
  "anya-e1005n0:804": "Na lābuṃ navamaṃ tathā;",
  "anya-e1005n0:805": "Dvādasa prinnaṃtriminaṃ,",
  "anya-e1005n0:806": "Bhuñje sippaṃ vinassati.",
  "anya-e1005n0:807": "156.",
  "anya-e1005n0:808": "Ekaṃ caje kulaatthaṃ,",
  "anya-e1005n0:809": "Gāmassatthaṃ kuvaṃ caje;",
  "anya-e1005n0:810": "Gāma caje janapadatthaṃ,",
  "anya-e1005n0:811": "Attatthaṃ pathaviṃ caje.",
  "anya-e1005n0:812": "157.",
  "anya-e1005n0:813": "Desaṃ ossajja gacchanti,",
  "anya-e1005n0:814": "Sīho sappuriso gajo;",
  "anya-e1005n0:815": "Tattheva nidhanaṃ yanti,",
  "anya-e1005n0:816": "Kāko kāpuriso migo.",
  "anya-e1005n0:817": "158.",
  "anya-e1005n0:818": "Yamhi padese na māno,",
  "anya-e1005n0:819": "Na pemaṃ na ca bandhavā;",
  "anya-e1005n0:820": "Na ca vijjāgāho koci,",
  "anya-e1005n0:821": "Na tattha vasanaṃ kare.",
  "anya-e1005n0:822": "159.",
  "anya-e1005n0:823": "Caratyekena pādena,",
  "anya-e1005n0:824": "Tiṭṭhatyekena paṇḍito;",
  "anya-e1005n0:825": "A nisamma paraṃ ṭhānaṃ,",
  "anya-e1005n0:826": "Na pubbamālayaṃ jahe.",
  "anya-e1005n0:827": "160.",
  "anya-e1005n0:828": "Dhana dhañña payogesu,",
  "anya-e1005n0:829": "Tathā vijjāgamesu ca;",
  "anya-e1005n0:830": "Dūtesu apacāresu,",
  "anya-e1005n0:831": "Cajjā lajjā tadā bhave.",
  "anya-e1005n0:832": "161.",
  "anya-e1005n0:833": "Dvi guṇo thīnamāhāro,",
  "anya-e1005n0:834": "Buddhicāpi catugguṇo;",
  "anya-e1005n0:835": "Chagguṇo hoti vāyāmo,",
  "anya-e1005n0:836": "Kāmotvaṭṭha-guṇo bhave.",
  "anya-e1005n0:837": "162.",
  "anya-e1005n0:838": "Pabbe pabbe kamenucchu,",
  "anya-e1005n0:839": "Visesarasavāggato;",
  "anya-e1005n0:840": "Tathā sumettiko sādhu,",
  "anya-e1005n0:841": "Viparītova dujjano.",
  "anya-e1005n0:842": "163.",
  "anya-e1005n0:843": "Kassako vāṇijo macco,",
  "anya-e1005n0:844": "Samaṇo suta sīlavā;",
  "anya-e1005n0:845": "Tesu vipula jātesu,",
  "anya-e1005n0:846": "Raṭṭhampi vipulaṃ siyā.",
  "anya-e1005n0:847": "164.",
  "anya-e1005n0:848": "Asajjhāya malā mantā,",
  "anya-e1005n0:849": "Anuṭṭhāna malā gharā;",
  "anya-e1005n0:850": "Malaṃ vaṇṇassa kosajjaṃ,",
  "anya-e1005n0:851": "Pamādo rakkhato malaṃ.",
  "anya-e1005n0:852": "165.",
  "anya-e1005n0:853": "Hīnānaṃ gacchate vittaṃ,",
  "anya-e1005n0:854": "Vīrānaṃ santakattanaṃ;",
  "anya-e1005n0:855": "Vadanti ca hīnā janā,",
  "anya-e1005n0:856": "Pubba-kammappadhānakā.",
  "anya-e1005n0:857": "166.",
  "anya-e1005n0:858": "Na vadanti cevaṃdhīrā,",
  "anya-e1005n0:859": "Vāyamiṃsu sabbakamme;",
  "anya-e1005n0:860": "Na ce sijjhati taṃ kammaṃ,",
  "anya-e1005n0:861": "A-phalaṃ eva ko doso.",
  "anya-e1005n0:862": "167.",
  "anya-e1005n0:863": "Nīcaṃ kulaṃ nipaññaṃ vā,",
  "anya-e1005n0:864": "Nirūpaṃ nibalaṃ samaṃ;",
  "anya-e1005n0:865": "Imaṃ kālaṃ chuttakālaṃ,",
  "anya-e1005n0:866": "Dhanameva visesakaṃ.",
  "anya-e1005n0:867": "Pakiṇṇakakaṇḍo niṭṭhito.",
  "anya-e1005n0:868": "Paṇḍito sujano kaṇḍo,",
  "anya-e1005n0:869": "Dujjano mitta-itthi ca;",
  "anya-e1005n0:870": "Rājā pakiṇṇako cāti,",
  "anya-e1005n0:871": "Satta-kaṇḍa-vibhūsitaṃ.",
  "anya-e1005n0:872": "Visuddhā cāra-therena,",
  "anya-e1005n0:873": "Visuddhārāma-vāsinā;",
  "anya-e1005n0:874": "Sabba-kulānamatthāya,",
  "anya-e1005n0:875": "Visodhitaṃ pathakkhaye."
}