{
  "anya-e1005n0:0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1005n0:1": "Lokanīti",
  "anya-e1005n0:2": "1. Paṇḍitakaṇḍo",
  "anya-e1005n0:3": "1.",
  "anya-e1005n0:4": "Lokanītiṁ pavakkhāmi,",
  "anya-e1005n0:5": "Nānāsatthasamuddhaṭaṁ;",
  "anya-e1005n0:6": "Māgadheneva saṅkhepaṁ,",
  "anya-e1005n0:7": "Vanditvā ratanatthayaṁ.",
  "anya-e1005n0:8": "2.",
  "anya-e1005n0:9": "Nītiloke purisassa sāro,",
  "anya-e1005n0:10": "Mātā pitā ācariyoca mitto;",
  "anya-e1005n0:11": "Tasmā hi nītiṁ puriso vijaññā,",
  "anya-e1005n0:12": "Ñāṇī mahā hoti bahussutoca.",
  "anya-e1005n0:13": "3.",
  "anya-e1005n0:14": "Alasassa kuto sippaṁ,",
  "anya-e1005n0:15": "Asippassa kuto dhanaṁ;",
  "anya-e1005n0:16": "Adhanassa kuto mittaṁ,",
  "anya-e1005n0:17": "Amittassa kuto sukhaṁ;",
  "anya-e1005n0:18": "Asukhassa kuto puññaṁ,",
  "anya-e1005n0:19": "Apuññassa kuto varaṁ.",
  "anya-e1005n0:20": "4.",
  "anya-e1005n0:21": "Sippā samaṁ dhanaṁ natthi,",
  "anya-e1005n0:22": "Sippaṁ corā nagaṇhare;",
  "anya-e1005n0:23": "Idha loke sippaṁ mittaṁ,",
  "anya-e1005n0:24": "Paraloke sukhāvahaṁ.",
  "anya-e1005n0:25": "5.",
  "anya-e1005n0:26": "Appakaṁ nātimaññeyya,",
  "anya-e1005n0:27": "Citte sutaṁ nidhāpaye;",
  "anya-e1005n0:28": "Vammikodakabindūva,",
  "anya-e1005n0:29": "Cirena paripūrati.",
  "anya-e1005n0:30": "6.",
  "anya-e1005n0:31": "Khuddoti nātimaññeyya,",
  "anya-e1005n0:32": "Vijjaṁ vā sippameva vā;",
  "anya-e1005n0:33": "Ekampi pariyodātaṁ,",
  "anya-e1005n0:34": "Jīvitakappakāraṇaṁ.",
  "anya-e1005n0:35": "7.",
  "anya-e1005n0:36": "Sele sele na māṇikaṁ,",
  "anya-e1005n0:37": "Gaje gaje na muttikaṁ;",
  "anya-e1005n0:38": "Vane vane na candanaṁ,",
  "anya-e1005n0:39": "Ṭhāne ṭhāne na paṇḍitaṁ.",
  "anya-e1005n0:40": "8.",
  "anya-e1005n0:41": "Paṇḍito sutasampanno,",
  "anya-e1005n0:42": "Yattha atthīti ce suto;",
  "anya-e1005n0:43": "Mahussāhena taṁ ṭhānaṁ,",
  "anya-e1005n0:44": "Gantabbaṁva sutesinā.",
  "anya-e1005n0:45": "9.",
  "anya-e1005n0:46": "Sine sippaṁ sine dhanaṁ,",
  "anya-e1005n0:47": "Sine pabbatamāruhaṁ;",
  "anya-e1005n0:48": "Sine kāmassa kodhassa,",
  "anya-e1005n0:49": "Ime pañca sine sine.",
  "anya-e1005n0:50": "10.",
  "anya-e1005n0:51": "Suti sammuti saṅkhyāca,",
  "anya-e1005n0:52": "Yogā nīti visesakā;",
  "anya-e1005n0:53": "Gandhabbā gaṇikā ceva,",
  "anya-e1005n0:54": "Dhanu bedā ca pūraṇā.",
  "anya-e1005n0:55": "11.",
  "anya-e1005n0:56": "Tikicchā itihāsā ca,",
  "anya-e1005n0:57": "Joti māyā ca chandati;",
  "anya-e1005n0:58": "Ketu mantā ca saddā ca,",
  "anya-e1005n0:59": "Sippāṭṭhārasakā ime.",
  "anya-e1005n0:60": "12.",
  "anya-e1005n0:61": "Apuṭṭho paṇḍito bherī,",
  "anya-e1005n0:62": "Pajjunno me hoti pucchito;",
  "anya-e1005n0:63": "Bālo puṭṭho apuṭṭhopi,",
  "anya-e1005n0:64": "Bahumpi bhaṇate sadā.",
  "anya-e1005n0:65": "13.",
  "anya-e1005n0:66": "Potthakesu ca yaṁ sippaṁ,",
  "anya-e1005n0:67": "Parahatthesu yaṁ dhanaṁ;",
  "anya-e1005n0:68": "Yathākicce samuppanne,",
  "anya-e1005n0:69": "Na taṁ sippaṁ na taṁ dhanaṁ.",
  "anya-e1005n0:70": "14.",
  "anya-e1005n0:71": "Jalappamāṇaṁ kumuddanālaṁ,",
  "anya-e1005n0:72": "Kulappamāṇaṁ vinayo pamāṇaṁ;",
  "anya-e1005n0:73": "Byattippamāṇaṁ kathītavākyaṁ,",
  "anya-e1005n0:74": "Pathaviyā pamāṇaṁ tiṇa milātaṁ.",
  "anya-e1005n0:75": "15.",
  "anya-e1005n0:76": "Appassuto sutaṁ appaṁ,",
  "anya-e1005n0:77": "Bahuṁ maññati mānavā;",
  "anya-e1005n0:78": "Sindhūdakaṁ apassanto,",
  "anya-e1005n0:79": "Kūpe toyaṁva maṇḍuko.",
  "anya-e1005n0:80": "16.",
  "anya-e1005n0:81": "Pathamaṁ parājaye sippaṁ,",
  "anya-e1005n0:82": "Dutiyaṁ parājaye dhanaṁ;",
  "anya-e1005n0:83": "Tatiyaṁ parājaye dhammaṁ,",
  "anya-e1005n0:84": "Catutthaṁ kiṁ karissati.",
  "anya-e1005n0:85": "17.",
  "anya-e1005n0:86": "Byatta putra kimalaso,",
  "anya-e1005n0:87": "Abyatto bhārahārako;",
  "anya-e1005n0:88": "Byattako pūjito loke,",
  "anya-e1005n0:89": "Byatta putra dine dine.",
  "anya-e1005n0:90": "18.",
  "anya-e1005n0:91": "Mātā verī pitā satru,",
  "anya-e1005n0:92": "Kena bāle na sikkhitā;",
  "anya-e1005n0:93": "Sabhāmajjhe na sobhanti,",
  "anya-e1005n0:94": "Haṁsamajjhe bakoyathā.",
  "anya-e1005n0:95": "19.",
  "anya-e1005n0:96": "Kaṇṭakaṁ giri ko tikkhati,",
  "anya-e1005n0:97": "Ko añjanaṁ migakkhikaṁ;",
  "anya-e1005n0:98": "Uppathaṁ pallale ko sugandhaṁ,",
  "anya-e1005n0:99": "Kula-putta-rūpo ko pavattati;",
  "anya-e1005n0:100": "Sāmaṁ-bhāvo.",
  "anya-e1005n0:101": "20.",
  "anya-e1005n0:102": "Na rasaṁ akotambulaṁ,",
  "anya-e1005n0:103": "Adhanassa, laṅkatampi;",
  "anya-e1005n0:104": "Alonakantu byañjanaṁ,",
  "anya-e1005n0:105": "Byākaraṇaṁ asippassa.",
  "anya-e1005n0:106": "21.",
  "anya-e1005n0:107": "Sussusā sutasampanno,",
  "anya-e1005n0:108": "Sutāpaññāya pavaḍḍhati;",
  "anya-e1005n0:109": "Paññāya atthaṁ jānāti,",
  "anya-e1005n0:110": "Ñāto attho sukhāvaho.",
  "anya-e1005n0:111": "22.",
  "anya-e1005n0:112": "Bhojanaṁ methunaṁ niddā,",
  "anya-e1005n0:113": "Goṇe posepi vijjati;",
  "anya-e1005n0:114": "Vijjā viseso posassa,",
  "anya-e1005n0:115": "Taṁ hīno gosamo bhave.",
  "anya-e1005n0:116": "23.",
  "anya-e1005n0:117": "Natthi vijjāsamaṁmittaṁ,",
  "anya-e1005n0:118": "Naca byādhisamo ripu;",
  "anya-e1005n0:119": "Naca attasamaṁ pemaṁ,",
  "anya-e1005n0:120": "Naca kammasamaṁ balaṁ.",
  "anya-e1005n0:121": "24.",
  "anya-e1005n0:122": "Haṁso majjhe na kākānaṁ,",
  "anya-e1005n0:123": "Sīho gunnaṁ na sobhate;",
  "anya-e1005n0:124": "Gadrabhamajjhe turaṅgo,",
  "anya-e1005n0:125": "Bālamajjhe ca paṇḍito.",
  "anya-e1005n0:126": "25.",
  "anya-e1005n0:127": "Yāvajīvampi ce bālo,",
  "anya-e1005n0:128": "Paṇḍitaṁ payirupāsati;",
  "anya-e1005n0:129": "Na so dhammaṁ vijānāti,",
  "anya-e1005n0:130": "Dabbi sūparasaṁ yathā.",
  "anya-e1005n0:131": "26.",
  "anya-e1005n0:132": "Muhuttamapi ce viññū,",
  "anya-e1005n0:133": "Paṇḍitaṁ payirupāsati;",
  "anya-e1005n0:134": "Khippaṁ dhammaṁ vijānāti,",
  "anya-e1005n0:135": "Jivhā sūparasaṁ yathā.",
  "anya-e1005n0:136": "27.",
  "anya-e1005n0:137": "Vinā satthaṁ na gaccheyya,",
  "anya-e1005n0:138": "Sūro saṅgāmabhūmiyaṁ;",
  "anya-e1005n0:139": "Paṇḍitvāddhagū vāṇijo,",
  "anya-e1005n0:140": "Videsagamano tathā.",
  "anya-e1005n0:141": "28.",
  "anya-e1005n0:142": "Dhananāsaṁ manotāpaṁ,",
  "anya-e1005n0:143": "Ghare duccaritāni ca;",
  "anya-e1005n0:144": "Vañcanañca avamānaṁ,",
  "anya-e1005n0:145": "Paṇḍito na pakāsaye.",
  "anya-e1005n0:146": "29.",
  "anya-e1005n0:147": "Pattānurūpakaṁ vākyaṁ,",
  "anya-e1005n0:148": "Sabhāvarūpakaṁ piyaṁ;",
  "anya-e1005n0:149": "Attānurūpakaṁ kodhaṁ,",
  "anya-e1005n0:150": "Yo jānāti sa paṇḍito.",
  "anya-e1005n0:151": "30.",
  "anya-e1005n0:152": "A-dhanassa rasaṁ khādā,",
  "anya-e1005n0:153": "A-balassa hathā narā;",
  "anya-e1005n0:154": "A-paññassa vākya-kathā,",
  "anya-e1005n0:155": "Ummattaka-samā ime.",
  "anya-e1005n0:156": "31.",
  "anya-e1005n0:157": "Anavhāyaṁ gamayanto,",
  "anya-e1005n0:158": "A-pucchā bahu-bhāsako;",
  "anya-e1005n0:159": "Atta-guṇaṁ pakāsento,",
  "anya-e1005n0:160": "Ti-vidhaṁ hīna-lakkhaṇaṁ.",
  "anya-e1005n0:161": "32.",
  "anya-e1005n0:162": "Appa-rūpo bahuṁ bhāso,",
  "anya-e1005n0:163": "Appa-pañño pakāsito;",
  "anya-e1005n0:164": "Appa-pūro ghaṭo khobhe,",
  "anya-e1005n0:165": "Appa-khīrā gāvī cathe.",
  "anya-e1005n0:166": "33.",
  "anya-e1005n0:167": "Maṇḍūkepi ukre sīhe,",
  "anya-e1005n0:168": "Kākaggahe piye piye;",
  "anya-e1005n0:169": "A-paṇḍīpi paṇḍī hutvā,",
  "anya-e1005n0:170": "Dhīrā pucche vaye vaye.",
  "anya-e1005n0:171": "34.",
  "anya-e1005n0:172": "Maṇḍūkepi ukre sīhe,",
  "anya-e1005n0:173": "Sūkarepi uhe dīpe;",
  "anya-e1005n0:174": "Biḷāre sadise byagghe,",
  "anya-e1005n0:175": "Sabba dhīre sippa-same.",
  "anya-e1005n0:176": "35.",
  "anya-e1005n0:177": "Na titti rājā dhanamhi,",
  "anya-e1005n0:178": "Paṇḍitopi su-bhāsite;",
  "anya-e1005n0:179": "Cakkhuṁpi piya-dassane,",
  "anya-e1005n0:180": "Jale sāgaro na titti.",
  "anya-e1005n0:181": "36.",
  "anya-e1005n0:182": "Rūpa-yobbana-sampannā,",
  "anya-e1005n0:183": "Visātha-kutha-sambhavā;",
  "anya-e1005n0:184": "Vijjā-hīnā na sobhanti,",
  "anya-e1005n0:185": "Nigandhā iva kiṁsukā.",
  "anya-e1005n0:186": "37.",
  "anya-e1005n0:187": "Hīne putto rājāmacco,",
  "anya-e1005n0:188": "Bāla-putto ca paṇḍito;",
  "anya-e1005n0:189": "A-dhanassa dhanaṁ bahu,",
  "anya-e1005n0:190": "Purisānaṁ na maññatha.",
  "anya-e1005n0:191": "38.",
  "anya-e1005n0:192": "Yo sippa-lobhena,",
  "anya-e1005n0:193": "Bahuṁ gaṇhāti taṁ sippaṁ;",
  "anya-e1005n0:194": "Mūgova supinaṁ passaṁ,",
  "anya-e1005n0:195": "Kathetumpi na ussahe.",
  "anya-e1005n0:196": "39.",
  "anya-e1005n0:197": "Bhijjetuṁ kumbhakāro,",
  "anya-e1005n0:198": "Sobhetuṁ kumbha ghaṭṭati;",
  "anya-e1005n0:199": "Na khipituṁ apāyesu,",
  "anya-e1005n0:200": "Sissānaṁ vuḍḍhi-kāraṇā.",
  "anya-e1005n0:201": "40.",
  "anya-e1005n0:202": "Taggarañca palāsena,",
  "anya-e1005n0:203": "Yo naro upanayhati;",
  "anya-e1005n0:204": "Pattāpi surabhi vāyanti,",
  "anya-e1005n0:205": "Evaṁ dhīrūpasevanā.",
  "anya-e1005n0:206": "Paṇḍitakaṇḍo niṭṭhito."
}