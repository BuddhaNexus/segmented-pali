{
  "ja248:0.1": "Jātaka ",
  "ja248:0.2": "Dukanipāta ",
  "ja248:0.3": "Siṅgālavagga ",
  "ja248:0.4": "8. Kiṁsukopamajātaka ",
  "ja248:1.1": "“Sabbehi kiṁsuko diṭṭho, ",
  "ja248:1.2": "kinnvettha vicikicchatha; ",
  "ja248:1.3": "Na hi sabbesu ṭhānesu, ",
  "ja248:1.4": "sārathī paripucchito”. ",
  "ja248:2.1": "“Evaṁ sabbehi ñāṇehi, ",
  "ja248:2.2": "yesaṁ dhammā ajānitā; ",
  "ja248:2.3": "Te ve dhammesu kaṅkhanti, ",
  "ja248:2.4": "kiṁsukasmiṁva bhātaro”ti. ",
  "ja248:3.1": "Kiṁsukopamajātakaṁ aṭṭhamaṁ. "
}