{
  "tha-ap166:0.1": "Therāpadāna ",
  "tha-ap166:0.2": "Supāricariyavagga ",
  "tha-ap166:0.3": "4. Desapūjakattheraapadāna ",
  "tha-ap166:1.1": "“Atthadassī tu bhagavā, ",
  "tha-ap166:1.2": "lokajeṭṭho narāsabho; ",
  "tha-ap166:1.3": "Abbhuggantvāna vehāsaṁ, ",
  "tha-ap166:1.4": "gacchate anilañjase. ",
  "tha-ap166:2.1": "Yamhi dese ṭhito satthā, ",
  "tha-ap166:2.2": "abbhuggacchi mahāmuni; ",
  "tha-ap166:2.3": "Tāhaṁ desaṁ apūjesiṁ, ",
  "tha-ap166:2.4": "pasanno sehi pāṇibhi. ",
  "tha-ap166:3.1": "Aṭṭhārase kappasate, ",
  "tha-ap166:3.2": "addasaṁ yaṁ mahāmuniṁ; ",
  "tha-ap166:3.3": "Duggatiṁ nābhijānāmi, ",
  "tha-ap166:3.4": "desapūjāyidaṁ phalaṁ. ",
  "tha-ap166:4.1": "Ekādase kappasate, ",
  "tha-ap166:4.2": "gosujātasanāmako; ",
  "tha-ap166:4.3": "Sattaratanasampanno, ",
  "tha-ap166:4.4": "cakkavattī mahabbalo. ",
  "tha-ap166:5.1": "Paṭisambhidā catasso, ",
  "tha-ap166:5.2": "…pe… ",
  "tha-ap166:5.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap166:6.1": "Itthaṁ sudaṁ āyasmā desapūjako thero imā gāthāyo abhāsitthāti. ",
  "tha-ap166:7.1": "Desapūjakattherassāpadānaṁ catutthaṁ. "
}