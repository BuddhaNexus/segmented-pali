{
  "ja178:0.1": "Jātaka ",
  "ja178:0.2": "Dukanipāta ",
  "ja178:0.3": "Kalyāṇavagga ",
  "ja178:0.4": "8. Kacchapajātaka ",
  "ja178:1.1": "“Janittaṁ me bhavittaṁ me, ",
  "ja178:1.2": "iti paṅke avassayiṁ; ",
  "ja178:1.3": "Taṁ maṁ paṅko ajjhabhavi, ",
  "ja178:1.4": "yathā dubbalakaṁ tathā; ",
  "ja178:1.5": "Taṁ taṁ vadāmi bhaggava, ",
  "ja178:1.6": "suṇohi vacanaṁ mama”. ",
  "ja178:2.1": "“Gāme vā yadi vā raññe, ",
  "ja178:2.2": "sukhaṁ yatrādhigacchati; ",
  "ja178:2.3": "Taṁ janittaṁ bhavittañca, ",
  "ja178:2.4": "purisassa pajānato; ",
  "ja178:2.5": "Yamhi jīve tamhi gacche, ",
  "ja178:2.6": "na niketahato siyā”ti. ",
  "ja178:3.1": "Kacchapajātakaṁ aṭṭhamaṁ. "
}