{
  "ja107:0.1": "Jātaka ",
  "ja107:0.2": "Ekakanipāta ",
  "ja107:0.3": "Parosatavagga ",
  "ja107:0.4": "7. Sālittakajātaka ",
  "ja107:1.1": "“Sādhu kho sippakaṁ nāma, ",
  "ja107:1.2": "api yādisa kīdisaṁ; ",
  "ja107:1.3": "Passa khañjappahārena, ",
  "ja107:1.4": "laddhā gāmā catuddisā”ti. ",
  "ja107:2.1": "Sālittakajātakaṁ sattamaṁ. "
}