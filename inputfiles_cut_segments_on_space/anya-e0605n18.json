{
  "anya-e0605n18:0": "Akkharuttarikayamakagāthā",
  "anya-e0605n18:1": "105.",
  "anya-e0605n18:2": " Nonānino nanūnāni nanenāni nanānino,",
  "anya-e0605n18:3": "Nunnānenāni nūna na nānanaṁ nānanena no.",
  "anya-e0605n18:4": "106.",
  "anya-e0605n18:5": " Sāre surāsure sārī rasasārasarissaro,",
  "anya-e0605n18:6": "Rasasārarase sāri surāsurasarassire.",
  "anya-e0605n18:7": "107.",
  "anya-e0605n18:8": " Devānaṁ nandano devo devadeva na nandi no,",
  "anya-e0605n18:9": "Vedadīnena vedana vedi vedana vedino.",
  "anya-e0605n18:10": "108.",
  "anya-e0605n18:11": " Devāsane nisinno so devadevo sasāsane,",
  "anya-e0605n18:12": "Nisinnānaṁ sadevānaṁ desesi dassanāsanaṁ."
}