{
  "cp34:0.1": "Cariyāpiṭaka ",
  "cp34:0.2": "Yudhañjayavagga ",
  "cp34:0.3": "Mettāpāramī 2 ",
  "cp34:0.4": "Ekarājacariya ",
  "cp34:1.1": "“Punāparaṁ yadā homi, ",
  "cp34:1.2": "ekarājāti vissuto; ",
  "cp34:1.3": "Paramaṁ sīlaṁ adhiṭṭhāya, ",
  "cp34:1.4": "pasāsāmi mahāmahiṁ. ",
  "cp34:2.1": "Dasa kusalakammapathe, ",
  "cp34:2.2": "vattāmi anavasesato; ",
  "cp34:2.3": "Catūhi saṅgahavatthūhi, ",
  "cp34:2.4": "saṅgaṇhāmi mahājanaṁ. ",
  "cp34:3.1": "Evaṁ me appamattassa, ",
  "cp34:3.2": "idha loke parattha ca; ",
  "cp34:3.3": "Dabbaseno upagantvā, ",
  "cp34:3.4": "acchindanto puraṁ mama. ",
  "cp34:4.1": "Rājūpajīve nigame, ",
  "cp34:4.2": "sabalaṭṭhe saraṭṭhake; ",
  "cp34:4.3": "Sabbaṁ hatthagataṁ katvā, ",
  "cp34:4.4": "kāsuyā nikhaṇī mamaṁ. ",
  "cp34:5.1": "Amaccamaṇḍalaṁ rajjaṁ, ",
  "cp34:5.2": "phītaṁ antepuraṁ mama; ",
  "cp34:5.3": "Acchinditvāna gahitaṁ, ",
  "cp34:5.4": "piyaṁ puttaṁva passahaṁ; ",
  "cp34:5.5": "Mettāya me samo natthi, ",
  "cp34:5.6": "esā me mettāpāramī”ti. ",
  "cp34:6.1": "Ekarājacariyaṁ cuddasamaṁ. "
}