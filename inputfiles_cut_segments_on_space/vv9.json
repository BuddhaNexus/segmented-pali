{
  "vv9:0.1": "Vimānavatthu ",
  "vv9:0.2": "Itthivimāna ",
  "vv9:0.3": "Pīṭhavagga ",
  "vv9:0.4": "9. Dīpavimānavatthu ",
  "vv9:1.1": "“Abhikkantena vaṇṇena, ",
  "vv9:1.2": "yā tvaṁ tiṭṭhasi devate; ",
  "vv9:1.3": "Obhāsentī disā sabbā, ",
  "vv9:1.4": "osadhī viya tārakā. ",
  "vv9:2.1": "Kena tetādiso vaṇṇo, ",
  "vv9:2.2": "kena te idha mijjhati; ",
  "vv9:2.3": "Uppajjanti ca te bhogā, ",
  "vv9:2.4": "ye keci manaso piyā. ",
  "vv9:3.1": "Kena tvaṁ vimalobhāsā, ",
  "vv9:3.2": "atirocasi devatā; ",
  "vv9:3.3": "Kena te sabbagattehi, ",
  "vv9:3.4": "sabbā obhāsate disā. ",
  "vv9:4.1": "Pucchāmi taṁ devi mahānubhāve, ",
  "vv9:4.2": "Manussabhūtā kimakāsi puññaṁ; ",
  "vv9:4.3": "Kenāsi evaṁ jalitānubhāvā, ",
  "vv9:4.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti. ",
  "vv9:5.1": "Sā devatā attamanā, ",
  "vv9:5.2": "moggallānena pucchitā; ",
  "vv9:5.3": "Pañhaṁ puṭṭhā viyākāsi, ",
  "vv9:5.4": "yassa kammassidaṁ phalaṁ. ",
  "vv9:6.1": "“Ahaṁ manussesu manussabhūtā, ",
  "vv9:6.2": "Purimāya jātiyā manussaloke; ",
  "vv9:6.3": "Tamandhakāramhi timīsikāyaṁ, ",
  "vv9:6.4": "Padīpakālamhi adāsi dīpaṁ. ",
  "vv9:7.1": "Yo andhakāramhi timīsikāyaṁ, ",
  "vv9:7.2": "Padīpakālamhi dadāti dīpaṁ; ",
  "vv9:7.3": "Upapajjati jotirasaṁ vimānaṁ, ",
  "vv9:7.4": "Pahūtamalyaṁ bahupuṇḍarīkaṁ. ",
  "vv9:8.1": "Tena metādiso vaṇṇo, ",
  "vv9:8.2": "tena me idha mijjhati; ",
  "vv9:8.3": "Uppajjanti ca me bhogā, ",
  "vv9:8.4": "ye keci manaso piyā. ",
  "vv9:9.1": "Tenāhaṁ vimalobhāsā, ",
  "vv9:9.2": "atirocāmi devatā; ",
  "vv9:9.3": "Tena me sabbagattehi, ",
  "vv9:9.4": "sabbā obhāsate disā. ",
  "vv9:10.1": "Akkhāmi te bhikkhu mahānubhāva, ",
  "vv9:10.2": "Manussabhūtā yamakāsi puññaṁ; ",
  "vv9:10.3": "Tenamhi evaṁ jalitānubhāvā, ",
  "vv9:10.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti. ",
  "vv9:11.1": "Dīpavimānaṁ navamaṁ. "
}