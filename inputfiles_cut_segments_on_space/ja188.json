{
  "ja188:0.1": "Jātaka ",
  "ja188:0.2": "Dukanipāta ",
  "ja188:0.3": "Asadisavagga ",
  "ja188:0.4": "8. Sīhakotthujātaka ",
  "ja188:1.1": "“Sīhaṅgulī sīhanakho, ",
  "ja188:1.2": "sīhapādapatiṭṭhito; ",
  "ja188:1.3": "So sīho sīhasaṅghamhi, ",
  "ja188:1.4": "eko nadati aññathā”. ",
  "ja188:2.1": "“Mā tvaṁ nadi rājaputta, ",
  "ja188:2.2": "appasaddo vane vasa; ",
  "ja188:2.3": "Sarena kho taṁ jāneyyuṁ, ",
  "ja188:2.4": "na hi te pettiko saro”ti. ",
  "ja188:3.1": "Sīhakotthujātakaṁ aṭṭhamaṁ. "
}