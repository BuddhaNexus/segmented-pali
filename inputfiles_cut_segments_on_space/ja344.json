{
  "ja344:0.1": "Jātaka ",
  "ja344:0.2": "Catukkanipāta ",
  "ja344:0.3": "Cūḷakuṇālavagga ",
  "ja344:0.4": "4. Ambajātaka ",
  "ja344:1.1": "“Yo nīliyaṁ maṇḍayati, ",
  "ja344:1.2": "saṇḍāsena vihaññati; ",
  "ja344:1.3": "Tassa sā vasamanvetu, ",
  "ja344:1.4": "yā te ambe avāhari”. ",
  "ja344:2.1": "“Vīsaṁ vā pañcavīsaṁ vā, ",
  "ja344:2.2": "ūnatiṁsaṁ va jātiyā; ",
  "ja344:2.3": "Tādisā pati mā laddhā, ",
  "ja344:2.4": "yā te ambe avāhari”. ",
  "ja344:3.1": "“Dīghaṁ gacchatu addhānaṁ, ",
  "ja344:3.2": "ekikā abhisārikā; ",
  "ja344:3.3": "Saṅkete pati mā adda, ",
  "ja344:3.4": "yā te ambe avāhari”. ",
  "ja344:4.1": "“Alaṅkatā suvasanā, ",
  "ja344:4.2": "mālinī candanussadā; ",
  "ja344:4.3": "Ekikā sayane setu, ",
  "ja344:4.4": "yā te ambe avāharī”ti. ",
  "ja344:5.1": "Ambajātakaṁ catutthaṁ. "
}