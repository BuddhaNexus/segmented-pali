{
  "ja276:0.1": "Jātaka ",
  "ja276:0.2": "Tikanipāta ",
  "ja276:0.3": "Udapānavagga ",
  "ja276:0.4": "6. Garudhammajātaka ",
  "ja276:1.1": "“Tava saddhañca sīlañca, ",
  "ja276:1.2": "viditvāna janādhipa; ",
  "ja276:1.3": "Vaṇṇaṁ añjanavaṇṇena, ",
  "ja276:1.4": "kāliṅgasmiṁ nimimhase”. ",
  "ja276:2.1": "“Annabhaccā cabhaccā ca, ",
  "ja276:2.2": "yodha uddissa gacchati; ",
  "ja276:2.3": "Sabbe te appaṭikkhippā, ",
  "ja276:2.4": "pubbācariyavaco idaṁ. ",
  "ja276:3.1": "Dadāmi vo brāhmaṇā nāgametaṁ, ",
  "ja276:3.2": "Rājārahaṁ rājabhoggaṁ yasassinaṁ; ",
  "ja276:3.3": "Alaṅkataṁ hemajālābhichannaṁ, ",
  "ja276:3.4": "Sasārathiṁ gacchatha yena kāman”ti. ",
  "ja276:4.1": "Garudhammajātakaṁ chaṭṭhaṁ. "
}