{
  "anya-e0809n0:0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e0809n0:1": "Abhidhānappadīpikā",
  "anya-e0809n0:2": "Buddhappaṇāmo",
  "anya-e0809n0:3": "1.",
  "anya-e0809n0:4": " Tathāgato yo karuṇākaro karo,",
  "anya-e0809n0:5": "Payāta’mosajja sukhappadaṁ padaṁ;",
  "anya-e0809n0:6": "Akā paratthaṁ kalisambhave bhave,",
  "anya-e0809n0:7": "Namāmi taṁ kevaladukkaraṁ karaṁ.",
  "anya-e0809n0:8": "Dhammappaṇāmo",
  "anya-e0809n0:9": "2.",
  "anya-e0809n0:10": " Apūjayuṁ yaṁ munikuñjarā jarā,",
  "anya-e0809n0:11": "Rujādimuttā yahimuttare tare;",
  "anya-e0809n0:12": "Ṭhitā tivaṭṭambunidhiṁ narānarā,",
  "anya-e0809n0:13": "Tariṁsu taṁ dhammamaghappahaṁ pahaṁ.",
  "anya-e0809n0:14": "Saṅghappaṇāmo",
  "anya-e0809n0:15": "3.",
  "anya-e0809n0:16": " Gataṁ munindo’rasasūnutaṁ nutaṁ,",
  "anya-e0809n0:17": "Supuññakhettaṁ bhuvane sutaṁ sutaṁ;",
  "anya-e0809n0:18": "Gaṇampi pāṇīkatasaṁvaraṁ varaṁ,",
  "anya-e0809n0:19": "Sadā guṇoghena nirantara’ntaraṁ.",
  "anya-e0809n0:20": "Paṭiññā",
  "anya-e0809n0:21": "4.",
  "anya-e0809n0:22": " Nāmaliṅgesu kosalla, matthanicchayakāraṇaṁ;",
  "anya-e0809n0:23": "Yato mahabbalaṁ buddha, vacane pāṭavatthinaṁ.",
  "anya-e0809n0:24": "5.",
  "anya-e0809n0:25": " Nāmaliṅgānya’to buddha, bhāsitassā’rahānya’haṁ;",
  "anya-e0809n0:26": "Dassayanto pakāsessa, mabhidhānappadīpikaṁ.",
  "anya-e0809n0:27": "Paribhāsā",
  "anya-e0809n0:28": "6.",
  "anya-e0809n0:29": " Bhiyyo rūpantarā sāha, cariyena ca katthaci;",
  "anya-e0809n0:30": "Kvacā’ haccavidhānena ñeyyaṁ thīpunnapuṁsakaṁ.",
  "anya-e0809n0:31": "7.",
  "anya-e0809n0:32": " Abhinnaliṅgānaṁyeva, dvando ca, liṅgavācakā;",
  "anya-e0809n0:33": "Gāthāpādantamajjhaṭṭhā, pubbaṁ yantyapare paraṁ.",
  "anya-e0809n0:34": "8.",
  "anya-e0809n0:35": " Pumitthiyaṁ padaṁ dvīsu, sabbaliṅge ca tīsviti;",
  "anya-e0809n0:36": "Abhidhānantarārambhe, ñeyyaṁ tva’nta mathādi ca.",
  "anya-e0809n0:37": "9.",
  "anya-e0809n0:38": " Bhiyyo payoga māgamma, sogate āgame kvaci;",
  "anya-e0809n0:39": "Nighaṇḍu yutti ñcānīya, nāmaliṅgaṁ kathīyatīti."
}