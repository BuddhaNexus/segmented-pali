{
  "ja398:0.1": "Jātaka ",
  "ja398:0.2": "Sattakanipāta ",
  "ja398:0.3": "Kukkuvagga ",
  "ja398:0.4": "3. Sutanujātaka ",
  "ja398:1.1": "“Rājā te bhattaṁ pāhesi, ",
  "ja398:1.2": "Suciṁ maṁsūpasecanaṁ; ",
  "ja398:1.3": "Maghadevasmiṁ adhivatthe, ",
  "ja398:1.4": "Ehi nikkhamma bhuñjasu”. ",
  "ja398:2.1": "“Ehi māṇava orena, ",
  "ja398:2.2": "bhikkhamādāya sūpitaṁ; ",
  "ja398:2.3": "Tvañca māṇava bhikkhā ca, ",
  "ja398:2.4": "ubho bhakkhā bhavissatha”. ",
  "ja398:3.1": "“Appakena tuvaṁ yakkha, ",
  "ja398:3.2": "thullamatthaṁ jahissasi; ",
  "ja398:3.3": "Bhikkhaṁ te nāharissanti, ",
  "ja398:3.4": "janā maraṇasaññino. ",
  "ja398:4.1": "Laddhāya yakkhā tava niccabhikkhaṁ, ",
  "ja398:4.2": "Suciṁ paṇītaṁ rasasā upetaṁ; ",
  "ja398:4.3": "Bhikkhañca te āhariyo naro idha, ",
  "ja398:4.4": "Sudullabho hehiti bhakkhite mayi”. ",
  "ja398:5.1": "“Mameva sutano attho, ",
  "ja398:5.2": "yathā bhāsasi māṇava; ",
  "ja398:5.3": "Mayā tvaṁ samanuññāto, ",
  "ja398:5.4": "sotthiṁ passāhi mātaraṁ. ",
  "ja398:6.1": "Khaggaṁ chattañca pātiñca, ",
  "ja398:6.2": "gacchamādāya māṇava; ",
  "ja398:6.3": "Sotthiṁ passatu te mātā, ",
  "ja398:6.4": "tvañca passāhi mātaraṁ”. ",
  "ja398:7.1": "“Evaṁ yakkha sukhī hohi, ",
  "ja398:7.2": "saha sabbehi ñātibhi; ",
  "ja398:7.3": "Dhanañca me adhigataṁ, ",
  "ja398:7.4": "rañño ca vacanaṁ katan”ti. ",
  "ja398:8.1": "Sutanujātakaṁ tatiyaṁ. "
}