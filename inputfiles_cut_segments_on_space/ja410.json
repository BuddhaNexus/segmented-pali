{
  "ja410:0.1": "Jātaka ",
  "ja410:0.2": "Sattakanipāta ",
  "ja410:0.3": "Gandhāravagga ",
  "ja410:0.4": "5. Somadattajātaka ",
  "ja410:1.1": "“Yo maṁ pure paccuḍḍeti, ",
  "ja410:1.2": "araññe dūramāyato; ",
  "ja410:1.3": "So na dissati mātaṅgo, ",
  "ja410:1.4": "somadatto kuhiṁ gato. ",
  "ja410:2.1": "Ayaṁ vā so mato seti, ",
  "ja410:2.2": "allasiṅgaṁva vacchito; ",
  "ja410:2.3": "Bhumyā nipatito seti, ",
  "ja410:2.4": "amarā vata kuñjaro”. ",
  "ja410:3.1": "“Anagāriyupetassa, ",
  "ja410:3.2": "vippamuttassa te sato; ",
  "ja410:3.3": "Samaṇassa na taṁ sādhu, ",
  "ja410:3.4": "yaṁ petamanusocasi”. ",
  "ja410:4.1": "“Saṁvāsena have sakka, ",
  "ja410:4.2": "manussassa migassa vā; ",
  "ja410:4.3": "Hadaye jāyate pemaṁ, ",
  "ja410:4.4": "taṁ na sakkā asocituṁ”. ",
  "ja410:5.1": "“Mataṁ marissaṁ rodanti, ",
  "ja410:5.2": "ye rudanti lapanti ca; ",
  "ja410:5.3": "Tasmā tvaṁ isi mā rodi, ",
  "ja410:5.4": "roditaṁ moghamāhu santo. ",
  "ja410:6.1": "Kanditena have brahme, ",
  "ja410:6.2": "mato peto samuṭṭhahe; ",
  "ja410:6.3": "Sabbe saṅgamma rodāma, ",
  "ja410:6.4": "aññamaññassa ñātake”. ",
  "ja410:7.1": "“Ādittaṁ vata maṁ santaṁ, ",
  "ja410:7.2": "ghatasittaṁva pāvakaṁ; ",
  "ja410:7.3": "Vārinā viya osiñcaṁ, ",
  "ja410:7.4": "sabbaṁ nibbāpaye daraṁ. ",
  "ja410:8.1": "Abbahī vata me sallaṁ, ",
  "ja410:8.2": "yamāsi hadayassitaṁ; ",
  "ja410:8.3": "Yo me sokaparetassa, ",
  "ja410:8.4": "puttasokaṁ apānudi. ",
  "ja410:9.1": "Sohaṁ abbūḷhasallosmi, ",
  "ja410:9.2": "vītasoko anāvilo; ",
  "ja410:9.3": "Na socāmi na rodāmi, ",
  "ja410:9.4": "tava sutvāna vāsavā”ti. ",
  "ja410:10.1": "Somadattajātakaṁ pañcamaṁ. "
}