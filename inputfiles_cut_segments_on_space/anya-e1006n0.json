{
  "anya-e1006n0:0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1006n0:1": "Suttantanīti",
  "anya-e1006n0:2": "1.",
  "anya-e1006n0:3": "Parā bhavantaṃ purisaṃ,",
  "anya-e1006n0:4": "Mayaṃ pucchāma gotamaṃ;",
  "anya-e1006n0:5": "Bhavantaṃ puṭṭhu māgamma,",
  "anya-e1006n0:6": "Kiṃ parābhavato mukhaṃ.",
  "anya-e1006n0:7": "2.",
  "anya-e1006n0:8": "Suvijāno bhavaṃhoti,",
  "anya-e1006n0:9": "Duvijāno parābhavo;",
  "anya-e1006n0:10": "Dhammakāmo bhavaṃ hoti,",
  "anya-e1006n0:11": "Dhammadessī parābhavo.",
  "anya-e1006n0:12": "3.",
  "anya-e1006n0:13": "Itihe taṃ vijānāma,",
  "anya-e1006n0:14": "Pathamo so parābhavo;",
  "anya-e1006n0:15": "Dutīyaṃ bhagavā brūhi,",
  "anya-e1006n0:16": "Kiṃ parābhavato mukhaṃ.",
  "anya-e1006n0:17": "4.",
  "anya-e1006n0:18": "Asantassa piyo hoti,",
  "anya-e1006n0:19": "Sante na kurute piyaṃ;",
  "anya-e1006n0:20": "Asataṃ dhammaṃ roceti,",
  "anya-e1006n0:21": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n0:22": "Kammāparādhasattānaṃ,",
  "anya-e1006n0:23": "Vināse paccupaṭṭhite;",
  "anya-e1006n0:24": "Anayo nayarūpena,",
  "anya-e1006n0:25": "Buddhimākamya tiṭṭhati.",
  "anya-e1006n0:26": "5.",
  "anya-e1006n0:27": "Niddāsīlī sabhāsīlī,",
  "anya-e1006n0:28": "Anuṭṭhātā ca yo naro;",
  "anya-e1006n0:29": "Alaso kodhapaññāṇo,",
  "anya-e1006n0:30": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n0:31": "6.",
  "anya-e1006n0:32": "Yo mātaraṃ pitaraṃ vā,",
  "anya-e1006n0:33": "Jiṇṇakaṃ gatayobbanaṃ;",
  "anya-e1006n0:34": "Pahusanto na bharati,",
  "anya-e1006n0:35": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n0:36": "7.",
  "anya-e1006n0:37": "Yo brāhmaṇaṃ samaṇaṃ vā,",
  "anya-e1006n0:38": "Aññaṃ vāpi vaṇibbakaṃ;",
  "anya-e1006n0:39": "Musāvādena vañceti,",
  "anya-e1006n0:40": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n0:41": "8.",
  "anya-e1006n0:42": "Pahutavitto puriso,",
  "anya-e1006n0:43": "Sahirañño sabhojano;",
  "anya-e1006n0:44": "Eko bhuñjati sādūni,",
  "anya-e1006n0:45": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n0:46": "9.",
  "anya-e1006n0:47": "Jātithaddho dhanathaddho,",
  "anya-e1006n0:48": "Gottathaddho ca yo naro;",
  "anya-e1006n0:49": "Sañātiṃ atimaññeti,",
  "anya-e1006n0:50": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n0:51": "10.",
  "anya-e1006n0:52": "Itthidhutto surādhutto,",
  "anya-e1006n0:53": "Akkhadhutto ca yo naro;",
  "anya-e1006n0:54": "Laddhaṃladdhaṃ vināseti,",
  "anya-e1006n0:55": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n0:56": "Na bāḷhaṃ itthiṃ gaccheyya,",
  "anya-e1006n0:57": "Sampassaṃ tejasaṅkhayaṃ;",
  "anya-e1006n0:58": "Kāsaṃ sāsaṃ daraṃ bālyaṃ,",
  "anya-e1006n0:59": "Khīṇamedo nigacchati.",
  "anya-e1006n0:60": "(Ka)",
  "anya-e1006n0:61": "Māyācetā marīcī ca,",
  "anya-e1006n0:62": "Soko rogo upaddavo;",
  "anya-e1006n0:63": "Kharā ca bandhanācetā,",
  "anya-e1006n0:64": "Maccupāso guhāsayo.",
  "anya-e1006n0:65": "(Kha)",
  "anya-e1006n0:66": "Balavanto dubbalā honti,",
  "anya-e1006n0:67": "Thāmavantopi hāyare;",
  "anya-e1006n0:68": "Cakkhumā andhakā honti,",
  "anya-e1006n0:69": "Mātugāmavasaṃgatā.",
  "anya-e1006n0:70": "(Ga)",
  "anya-e1006n0:71": "Guṇavanto nigguṇā honti,",
  "anya-e1006n0:72": "Paññavantopi hāyare;",
  "anya-e1006n0:73": "Pamuttā bandhanā senti,",
  "anya-e1006n0:74": "Mātugāmavasaṃgatā.",
  "anya-e1006n0:75": "(Gha)",
  "anya-e1006n0:76": "Yasaṃ kittiṃ dhitiṃ sūraṃ;",
  "anya-e1006n0:77": "Bāhussaccaṃ pajānanaṃ;",
  "anya-e1006n0:78": "Hāpayanti pamattassa;",
  "anya-e1006n0:79": "Kaṭṭhapuñcaṃva pāvako.",
  "anya-e1006n0:80": "11.",
  "anya-e1006n0:81": "Sehi dārehi santuṭṭho,",
  "anya-e1006n0:82": "Vesiyāsu padussati;",
  "anya-e1006n0:83": "Dussati paradāresu,",
  "anya-e1006n0:84": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n0:85": "(Ka)",
  "anya-e1006n0:86": "Mayañca bhariyaṃ nātikkamāma,",
  "anya-e1006n0:87": "Amheca bhariyā nātikkamanti;",
  "anya-e1006n0:88": "Aññatra tāhi brahmacariyaṃ carāma,",
  "anya-e1006n0:89": "Tasmā hi amhaṃ daharā na miyyare.",
  "anya-e1006n0:90": "(Kha)",
  "anya-e1006n0:91": "Etāsu jāyare suttamāsu,",
  "anya-e1006n0:92": "Medhāvino honti pahutapaññā;",
  "anya-e1006n0:93": "Bahussutā theraguṇā ca honti,",
  "anya-e1006n0:94": "Tasmā hi amhaṃ daharā na miyyare.",
  "anya-e1006n0:95": "12.",
  "anya-e1006n0:96": "Atītayobbano poso,",
  "anya-e1006n0:97": "Āneti timbarutthaniṃ;",
  "anya-e1006n0:98": "Tassa issā na supati,",
  "anya-e1006n0:99": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n0:100": "(Ka)",
  "anya-e1006n0:101": "Na dukkhaṃ ahinā daṭṭhaṃ,",
  "anya-e1006n0:102": "Na dukkhaṃ sattiyā hataṃ;",
  "anya-e1006n0:103": "Tañca dukkhañca tibbañca,",
  "anya-e1006n0:104": "Yaṃ passe jiṇṇakaṃ patiṃ.",
  "anya-e1006n0:105": "(Kha)",
  "anya-e1006n0:106": "Natthi khiṭṭā natthi rati,",
  "anya-e1006n0:107": "Jiṇṇena patinā saha;",
  "anya-e1006n0:108": "Natthi allāpasallāpo,",
  "anya-e1006n0:109": "Jagghituṃpi na sobhati.",
  "anya-e1006n0:110": "(Ga)",
  "anya-e1006n0:111": "Yadā ca daharo daharī,",
  "anya-e1006n0:112": "Mantayiṃsu rahogatā;",
  "anya-e1006n0:113": "Sabbe sokā vinassanti,",
  "anya-e1006n0:114": "Yekeci hadayassitā.",
  "anya-e1006n0:115": "13.",
  "anya-e1006n0:116": "Itthiṃ soṇḍiṃ vikiraṇiṃ,",
  "anya-e1006n0:117": "Purisaṃ vāpi tādisaṃ;",
  "anya-e1006n0:118": "Issariyasmiṃ thapeti,",
  "anya-e1006n0:119": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n0:120": "14.",
  "anya-e1006n0:121": "Appabhogo mahātaṇho,",
  "anya-e1006n0:122": "Khattiye jāyate kule;",
  "anya-e1006n0:123": "So ca rajjaṃ patthayati,",
  "anya-e1006n0:124": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n0:125": "“Suvijāno bhavaṃhoti,",
  "anya-e1006n0:126": "Duvijāno parābhavo”;",
  "anya-e1006n0:127": "Appabhogo mahātaṇho.",
  "anya-e1006n0:128": "15.",
  "anya-e1006n0:129": "Ete parābhave loke,",
  "anya-e1006n0:130": "Paṇḍito samavekkhiya;",
  "anya-e1006n0:131": "Ariyo dassanasampanno,",
  "anya-e1006n0:132": "Sa lokaṃ bhajate sivaṃ.",
  "anya-e1006n0:133": "Vasalasutta",
  "anya-e1006n0:134": "1.",
  "anya-e1006n0:135": "Kodhano upanāhīca,",
  "anya-e1006n0:136": "Pāpamakkhī ca yo naro;",
  "anya-e1006n0:137": "Vipannadiṭṭhī māyāvī,",
  "anya-e1006n0:138": "Taṃ jaññā vasalo iti.",
  "anya-e1006n0:139": "2.",
  "anya-e1006n0:140": "Ekajaṃ vā dvijaṃ vāpi,",
  "anya-e1006n0:141": "Yodha pāṇaṃ vihiṃsati;",
  "anya-e1006n0:142": "Yassa pāṇe dayā natthi,",
  "anya-e1006n0:143": "Taṃ jaññā vasalo iti.",
  "anya-e1006n0:144": "3.",
  "anya-e1006n0:145": "Yo hanti uparundhati,",
  "anya-e1006n0:146": "Gāmāni nigamāni ca;",
  "anya-e1006n0:147": "Niggāhako samaññāto,",
  "anya-e1006n0:148": "Taṃ jaññā vasalo iti.",
  "anya-e1006n0:149": "4.",
  "anya-e1006n0:150": "Gāme vā yadi vā raññe,",
  "anya-e1006n0:151": "Yaṃ paresaṃ mamāyitaṃ;",
  "anya-e1006n0:152": "Theyyā adinnaṃ ādeti,",
  "anya-e1006n0:153": "Taṃ jaññā vasalo iti.",
  "anya-e1006n0:154": "5.",
  "anya-e1006n0:155": "Yo have iṇamādāya,",
  "anya-e1006n0:156": "Vuccamāno palāyati;",
  "anya-e1006n0:157": "Na hi te iṇamatthīti,",
  "anya-e1006n0:158": "Taṃ jaññā vasalo iti.",
  "anya-e1006n0:159": "6.",
  "anya-e1006n0:160": "Yodha kiñcikkhakamyatā,",
  "anya-e1006n0:161": "Pathasmiṃ vajataṃ janaṃ;",
  "anya-e1006n0:162": "Hantvā kiñcikkha mādeti;",
  "anya-e1006n0:163": "Taṃ jaññā vasalo iti.",
  "anya-e1006n0:164": "7.",
  "anya-e1006n0:165": "Yo attahetu parahetu,",
  "anya-e1006n0:166": "Dhanahetu ca yo naro;",
  "anya-e1006n0:167": "Sakkhipuṭṭho musābrūti,",
  "anya-e1006n0:168": "Taṃ jaññā vasalo iti.",
  "anya-e1006n0:169": "8.",
  "anya-e1006n0:170": "Yo ñātīnaṃ sakhīnaṃ vā,",
  "anya-e1006n0:171": "Dāresu paṭidissati;",
  "anya-e1006n0:172": "Sahasā sampiyena vā,",
  "anya-e1006n0:173": "Taṃ jaññā vasalo iti.",
  "anya-e1006n0:174": "9.",
  "anya-e1006n0:175": "Yo 4 mātaraṃ pitaraṃ vā,",
  "anya-e1006n0:176": "Jiṇṇakaṃ gatayobbanaṃ;",
  "anya-e1006n0:177": "Pahusanto na bharati,",
  "anya-e1006n0:178": "Taṃ jaññā vasalo iti.",
  "anya-e1006n0:179": "10.",
  "anya-e1006n0:180": "Yo mātaraṃ pitaraṃ vā,",
  "anya-e1006n0:181": "Bhātaraṃ bhaginiṃ sassuṃ;",
  "anya-e1006n0:182": "Hanti roseti vācāya,",
  "anya-e1006n0:183": "Taṃ jaññā vasalo iti.",
  "anya-e1006n0:184": "11.",
  "anya-e1006n0:185": "Yo atthaṃ pucchito santo,",
  "anya-e1006n0:186": "Anattha manusāsati;",
  "anya-e1006n0:187": "Paṭicchannena manteti,",
  "anya-e1006n0:188": "Taṃ jaññā vasalo iti.",
  "anya-e1006n0:189": "12.",
  "anya-e1006n0:190": "Yo katvā pāpakaṃ kammaṃ,",
  "anya-e1006n0:191": "Māmaṃ jaññāti icchati;",
  "anya-e1006n0:192": "Yo paṭicchannakammanto,",
  "anya-e1006n0:193": "Taṃ jaññā vasalo iti.",
  "anya-e1006n0:194": "13.",
  "anya-e1006n0:195": "Yo ve parakulaṃ gantvā,",
  "anya-e1006n0:196": "Bhutvāna sucibhojanaṃ;",
  "anya-e1006n0:197": "Āgataṃ nappaṭipūjeti,",
  "anya-e1006n0:198": "Taṃ jaññā vasalo iti.",
  "anya-e1006n0:199": "14.",
  "anya-e1006n0:200": "Yo samaṇaṃ vā brāhmaṇaṃ,",
  "anya-e1006n0:201": "Aññaṃ vāpi vaṇibbakaṃ;",
  "anya-e1006n0:202": "Musāvādena vañceti,",
  "anya-e1006n0:203": "Taṃ jaññā vasalo iti.",
  "anya-e1006n0:204": "15.",
  "anya-e1006n0:205": "Yo samaṇaṃ vā brāhmaṇaṃ,",
  "anya-e1006n0:206": "Bhattakāle upaṭṭhitaṃ;",
  "anya-e1006n0:207": "Roseti vā na ca deti,",
  "anya-e1006n0:208": "Taṃ jaññā vasalo iti.",
  "anya-e1006n0:209": "16.",
  "anya-e1006n0:210": "Asantaṃ yodha pabrūti,",
  "anya-e1006n0:211": "Mohena paliguṇṭhito;",
  "anya-e1006n0:212": "Kiñcanaṃ nijigīsāno,",
  "anya-e1006n0:213": "Taṃ jaññā vasalo iti.",
  "anya-e1006n0:214": "17.",
  "anya-e1006n0:215": "Yocattānaṃ samukkaṃse,",
  "anya-e1006n0:216": "Pareca mavajānāti;",
  "anya-e1006n0:217": "Nihīno sena mānena,",
  "anya-e1006n0:218": "Taṃ jaññā vasalo iti.",
  "anya-e1006n0:219": "18.",
  "anya-e1006n0:220": "Rosako kadarīyo ca,",
  "anya-e1006n0:221": "Pāpiccho maccharī saṭho;",
  "anya-e1006n0:222": "Ahirīko anottappī,",
  "anya-e1006n0:223": "Taṃ jaññā vasalo iti.",
  "anya-e1006n0:224": "19.",
  "anya-e1006n0:225": "Yo buddhaṃ paribhāsati,",
  "anya-e1006n0:226": "Athavā tassa sāvakaṃ;",
  "anya-e1006n0:227": "Paribbajaṃ gahaṭṭhaṃ vā,",
  "anya-e1006n0:228": "Taṃ jaññā vasalo iti.",
  "anya-e1006n0:229_0": "Aṭṭhahi bhikkhave aṅgehi sampannāgatassa upāsakassa patto nikujjitabbo. Bhikkhūnaṃ",
  "anya-e1006n0:229_1": "alābhāya parisakkati, bhikkhūnaṃ anatthāya parisakkati, bhikkhūnaṃ anāvāsāya parisakkati,",
  "anya-e1006n0:229_2": "bhikkhūnaṃ akkosati paribhāsati, bhikkhūbhikkhūhi bhedeti, buddhassa avaṇṇaṃ bhāsati,",
  "anya-e1006n0:229_3": "dhammassa avaṇṇaṃ bhāsati, saṅghassa avaṇṇaṃ bhāsati, anujānāmi bhikkhave imehi aṭṭhahi",
  "anya-e1006n0:229_4": "aṅgehi sampannāgatassa upāsakassa pattaṃ nikujjituṃ.",
  "anya-e1006n0:230": "20.",
  "anya-e1006n0:231": "Yo anarahaṃ santo,",
  "anya-e1006n0:232": "Arahāti paṭijānāti;",
  "anya-e1006n0:233": "Coro sabrahmake loke,",
  "anya-e1006n0:234": "Taṃ jaññā vasalo iti.",
  "anya-e1006n0:235": "21.",
  "anya-e1006n0:236": "Ete kho vasalā vuttā,",
  "anya-e1006n0:237": "Mayā ye te pakāsitā;",
  "anya-e1006n0:238": "Na jaccā vasalo hoti,",
  "anya-e1006n0:239": "Na jaccā hoti brāhmaṇo.",
  "anya-e1006n0:240": "22.",
  "anya-e1006n0:241": "Kammunā vasalo hoti,",
  "anya-e1006n0:242": "Kammunā hoti brāhmaṇo.",
  "anya-e1006n0:243": "23.",
  "anya-e1006n0:244": "Tadamināpi jānātha,",
  "anya-e1006n0:245": "Yathāhetaṃ nidassanaṃ;",
  "anya-e1006n0:246": "Caṇḍālaputto sopāko,",
  "anya-e1006n0:247": "Mātaṅgo iti vissuto.",
  "anya-e1006n0:248": "24.",
  "anya-e1006n0:249": "So yasaṃ paramaṃ patto,",
  "anya-e1006n0:250": "Mātaṅgo yaṃ sudullabhaṃ;",
  "anya-e1006n0:251": "Āgacchuṃ tassupaṭṭhānaṃ,",
  "anya-e1006n0:252": "Khattiyā brāhmaṇā bahū.",
  "anya-e1006n0:253": "25.",
  "anya-e1006n0:254": "So devayānaṃ abhiruyha,",
  "anya-e1006n0:255": "Virajaṃ so mahāpathaṃ;",
  "anya-e1006n0:256": "Kāmarāgaṃ virājetvā,",
  "anya-e1006n0:257": "Brahmalokūpago ahu.",
  "anya-e1006n0:258": "Giriṃ nakhena khaṇasi,",
  "anya-e1006n0:259": "Ayo dantebhi khādasi;",
  "anya-e1006n0:260": "Jātavedaṃ padahasi,",
  "anya-e1006n0:261": "Yo isiṃ paribhāsati.",
  "anya-e1006n0:262": "Āvelitaṃ piṭṭhito uttamaṅgaṃ,",
  "anya-e1006n0:263": "Bāhuṃ pasāreti akampaṇeyyaṃ;",
  "anya-e1006n0:264": "Setāni akkhīni yathā matassa,",
  "anya-e1006n0:265": "Ko me imaṃ puttamakāsi evaṃ."
}