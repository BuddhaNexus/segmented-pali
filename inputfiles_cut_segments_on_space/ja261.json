{
  "ja261:0.1": "Jātaka ",
  "ja261:0.2": "Tikanipāta ",
  "ja261:0.3": "Padumavagga ",
  "ja261:0.4": "1. Padumajātaka ",
  "ja261:1.1": "“Yathā kesā ca massū ca, ",
  "ja261:1.2": "chinnaṁ chinnaṁ virūhati; ",
  "ja261:1.3": "Evaṁ rūhatu te nāsā, ",
  "ja261:1.4": "padumaṁ dehi yācito”. ",
  "ja261:2.1": "“Yathā sāradikaṁ bījaṁ, ",
  "ja261:2.2": "khette vuttaṁ virūhati; ",
  "ja261:2.3": "Evaṁ rūhatu te nāsā, ",
  "ja261:2.4": "padumaṁ dehi yācito”. ",
  "ja261:3.1": "“Ubhopi palapantete, ",
  "ja261:3.2": "api padmāni dassati; ",
  "ja261:3.3": "Vajjuṁ vā te na vā vajjuṁ, ",
  "ja261:3.4": "natthi nāsāya rūhanā; ",
  "ja261:3.5": "Dehi samma padumāni, ",
  "ja261:3.6": "ahaṁ yācāmi yācito”ti. ",
  "ja261:4.1": "Padumajātakaṁ paṭhamaṁ. "
}