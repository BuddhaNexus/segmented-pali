{
  "ja445:0.1": "Jātaka ",
  "ja445:0.2": "Dasakanipāta ",
  "ja445:0.3": "Catudvāravagga ",
  "ja445:0.4": "7. Nigrodhajātaka ",
  "ja445:1.1": "“Na vāhametaṁ jānāmi, ",
  "ja445:1.2": "ko vāyaṁ kassa vāti vā; ",
  "ja445:1.3": "Yathā sākho cari evaṁ, ",
  "ja445:1.4": "nigrodha kinti maññasi. ",
  "ja445:2.1": "Tato galavinītena, ",
  "ja445:2.2": "purisā nīhariṁsu maṁ; ",
  "ja445:2.3": "Datvā mukhapahārāni, ",
  "ja445:2.4": "sākhassa vacanaṅkarā. ",
  "ja445:3.1": "Etādisaṁ dummatinā, ",
  "ja445:3.2": "akataññuna dubbhinā; ",
  "ja445:3.3": "Kataṁ anariyaṁ sākhena, ",
  "ja445:3.4": "sakhinā te janādhipa”. ",
  "ja445:4.1": "“Na vāhametaṁ jānāmi, ",
  "ja445:4.2": "napi me koci saṁsati; ",
  "ja445:4.3": "Yaṁ me tvaṁ samma akkhāsi, ",
  "ja445:4.4": "sākhena kāraṇaṁ kataṁ. ",
  "ja445:5.1": "Sakhīnaṁ sājīvakaro, ",
  "ja445:5.2": "mama sākhassa cūbhayaṁ; ",
  "ja445:5.3": "Tvaṁ nosissariyaṁ dātā, ",
  "ja445:5.4": "manussesu mahantataṁ; ",
  "ja445:5.5": "Tayāmā labbhitā iddhī, ",
  "ja445:5.6": "ettha me natthi saṁsayo. ",
  "ja445:6.1": "Yathāpi bījamaggimhi, ",
  "ja445:6.2": "ḍayhati na virūhati; ",
  "ja445:6.3": "Evaṁ kataṁ asappurise, ",
  "ja445:6.4": "nassati na virūhati. ",
  "ja445:7.1": "Kataññumhi ca posamhi, ",
  "ja445:7.2": "Sīlavante ariyavuttine; ",
  "ja445:7.3": "Sukhette viya bījāni, ",
  "ja445:7.4": "Kataṁ tamhi na nassati”. ",
  "ja445:8.1": "“Imaṁ jammaṁ nekatikaṁ, ",
  "ja445:8.2": "asappurisacintakaṁ; ",
  "ja445:8.3": "Hanantu sākhaṁ sattīhi, ",
  "ja445:8.4": "nāssa icchāmi jīvitaṁ”. ",
  "ja445:9.1": "“Khamatassa mahārāja, ",
  "ja445:9.2": "Pāṇā na paṭiānayā; ",
  "ja445:9.3": "Khama deva asappurisassa, ",
  "ja445:9.4": "Nāssa icchāmahaṁ vadhaṁ”. ",
  "ja445:10.1": "“Nigrodhameva seveyya, ",
  "ja445:10.2": "na sākhamupasaṁvase; ",
  "ja445:10.3": "Nigrodhasmiṁ mataṁ seyyo, ",
  "ja445:10.4": "yañce sākhasmi jīvitan”ti. ",
  "ja445:11.1": "Nigrodhajātakaṁ sattamaṁ. "
}