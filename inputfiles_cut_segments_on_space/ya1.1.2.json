{
  "ya1.1.2:0.1": "1 Mūlayamaka",
  "ya1.1.2:0.2": "1.1 Uddesa",
  "ya1.1.2:0.3": "1.1.2. Hetuvārādiuddesa",
  "ya1.1.2:1.1_0": "Ye keci kusalā dhammā, sabbe te kusalahetū …pe… kusalanidānā …pe… kusalasambhavā",
  "ya1.1.2:1.1_1": "…pe… kusalappabhavā …pe… kusalasamuṭṭhānā …pe… kusalāhārā …pe… kusalārammaṇā …pe…",
  "ya1.1.2:1.1_2": "kusalapaccayā …pe… kusalasamudayā …pe….",
  "ya1.1.2:2.1": "Mūlaṁ hetu nidānañca,",
  "ya1.1.2:2.2": "Sambhavo pabhavena ca;",
  "ya1.1.2:2.3": "Samuṭṭhānāhārārammaṇā,",
  "ya1.1.2:2.4": "Paccayo samudayena cāti.",
  "ya1.1.2:3.1": "Uddesavāro niṭṭhito."
}