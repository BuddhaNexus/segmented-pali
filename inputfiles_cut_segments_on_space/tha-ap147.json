{
  "tha-ap147:0.1": "Therāpadāna ",
  "tha-ap147:0.2": "Chattavagga ",
  "tha-ap147:0.3": "5 Umāpupphiyattheraapadāna ",
  "tha-ap147:1.1": "“Nibbute lokamahite, ",
  "tha-ap147:1.2": "āhutīnaṁ paṭiggahe; ",
  "tha-ap147:1.3": "Siddhatthamhi bhagavati, ",
  "tha-ap147:1.4": "mahāthūpamaho ahu. ",
  "tha-ap147:2.1": "Mahe pavattamānamhi, ",
  "tha-ap147:2.2": "siddhatthassa mahesino; ",
  "tha-ap147:2.3": "Umāpupphaṁ gahetvāna, ",
  "tha-ap147:2.4": "thūpamhi abhiropayiṁ. ",
  "tha-ap147:3.1": "Catunnavutito kappe, ",
  "tha-ap147:3.2": "yaṁ pupphamabhiropayiṁ; ",
  "tha-ap147:3.3": "Duggatiṁ nābhijānāmi, ",
  "tha-ap147:3.4": "thūpapūjāyidaṁ phalaṁ. ",
  "tha-ap147:4.1": "Ito ca navame kappe, ",
  "tha-ap147:4.2": "somadevasanāmakā; ",
  "tha-ap147:4.3": "Pañcāsītisu rājāno, ",
  "tha-ap147:4.4": "cakkavattī mahabbalā. ",
  "tha-ap147:5.1": "Paṭisambhidā catasso, ",
  "tha-ap147:5.2": "…pe… ",
  "tha-ap147:5.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap147:6.1": "Itthaṁ sudaṁ āyasmā umāpupphiyo thero imā gāthāyo abhāsitthāti. ",
  "tha-ap147:7.1": "Umāpupphiyattherassāpadānaṁ pañcamaṁ. "
}