{
  "ja59:0.1": "Jātaka ",
  "ja59:0.2": "Ekakanipāta ",
  "ja59:0.3": "Āsīsavagga ",
  "ja59:0.4": "9. Bherivādakajātaka ",
  "ja59:1.1": "“Dhame dhame nātidhame, ",
  "ja59:1.2": "atidhantañhi pāpakaṁ; ",
  "ja59:1.3": "Dhantena hi sataṁ laddhaṁ, ",
  "ja59:1.4": "atidhantena nāsitan”ti. ",
  "ja59:2.1": "Bherivādakajātakaṁ navamaṁ. "
}