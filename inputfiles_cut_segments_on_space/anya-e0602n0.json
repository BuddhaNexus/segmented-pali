{
  "anya-e0602n0:0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e0602n0:1": "Mahāpaṇāmapāṭha",
  "anya-e0602n0:2": "(Buddhavandanā)",
  "anya-e0602n0:3": "1.",
  "anya-e0602n0:4": "So, ko; Ne, saṁ;",
  "anya-e0602n0:5": "2.",
  "anya-e0602n0:6": "Satthā, nesaṁ; Khemaṁ, dātā;",
  "anya-e0602n0:7": "3.",
  "anya-e0602n0:8": "Heḷāta-kkāro so; Jantūnaṁ, no mā nu;",
  "anya-e0602n0:9": "4.",
  "anya-e0602n0:10": "So lokandhaṁ, hantvā nakko;",
  "anya-e0602n0:11": "Sattambojaṁ, bodhetā nu.",
  "anya-e0602n0:12": "5.",
  "anya-e0602n0:13": "Jinasūriyo, tibhavanabhe;",
  "anya-e0602n0:14": "Muhatimiraṁ, abhitapaye.",
  "anya-e0602n0:15": "6.",
  "anya-e0602n0:16": "Jeno jinamūle, disvā janakāyaṁ;",
  "anya-e0602n0:17": "Saṁsāranimuggaṁ, sabbaññuta micchaṁ.",
  "anya-e0602n0:18": "7.",
  "anya-e0602n0:19": "Hitvā karupetaṁ, mokkhaṁ paṇidhānaṁ;",
  "anya-e0602n0:20": "Vattesi dayāṇā, yo taṁ muni vande.",
  "anya-e0602n0:21": "8.",
  "anya-e0602n0:22": "Sutāyubhariyā me, dhanaṅgamapi cajjaṁ.",
  "anya-e0602n0:23": "Cinaṁ budhinidāne, agā muniyaṁ metaṁ.",
  "anya-e0602n0:24": "9.",
  "anya-e0602n0:25": "So sasajātiya dehaṁ, cajjiya dānavarañca;",
  "anya-e0602n0:26": "Nāgusabho varasīlaṁ, pūrayi samparicāgo.",
  "anya-e0602n0:27": "10.",
  "anya-e0602n0:28": "Nekkhammaggaṁ rājā hutvā, senabbidvā paññāseṭṭhaṁ;",
  "anya-e0602n0:29": "Vedehindo vīrukkaṁsaṁ, khantīvādī khantīseṭṭhaṁ.",
  "anya-e0602n0:30": "11.",
  "anya-e0602n0:31": "So sutasomo tathagaṁ, temiya dhiṭṭhānavaraṁ;",
  "anya-e0602n0:32": "Ekabhugo mettavaraṁ, lomahasopekkhataraṁ.",
  "anya-e0602n0:33": "12.",
  "anya-e0602n0:34": "Pāramī tidukkarāva, pūriyāna seṭṭhabodhi;",
  "anya-e0602n0:35": "Pāpuṇittha yo ananta-dhammasāradaṁ namāmi.",
  "anya-e0602n0:36": "13.",
  "anya-e0602n0:37": "Pahāya cakkavattikaṁ, karopanīta maddiyaṁ;",
  "anya-e0602n0:38": "Bhusidhare sasenakaṁ, ajeni pāpimaṁ name.",
  "anya-e0602n0:39": "14.",
  "anya-e0602n0:40": "Athuttaraṁ jayākaraṁ, bahūpakārataṁ jino;",
  "anya-e0602n0:41": "Paṭicca nimmisakkhibhi, udikkhate namāmi taṁ.",
  "anya-e0602n0:42": "15.",
  "anya-e0602n0:43": "Caṅkame maruvimati yo, chindayaṁ kami rataniye;",
  "anya-e0602n0:44": "Iddhipāṭihiraṇakaro, cakkhumaṁ tamabhipaṇame.",
  "anya-e0602n0:45": "16.",
  "anya-e0602n0:46": "Atha ratanaghare buddho, chadidhiti janayaṁ saṅkhyaṁ;",
  "anya-e0602n0:47": "Sīmīsā tamabhidhammaṁ yo, suranaramahitaṁ vande.",
  "anya-e0602n0:48": "17.",
  "anya-e0602n0:49": "Nigrodhantika nubbhavī sukhaṁ, dhammaṁ yo vicinaṁ nadhīvaro;",
  "anya-e0602n0:50": "Kicchāladdha manaññabodhiyaṁ, vande taṁ sugataṁ nigrocaraṁ.",
  "anya-e0602n0:51": "18.",
  "anya-e0602n0:52": "Sabbhogehi tha mucalindena,",
  "anya-e0602n0:53": "Nāgindena rahasi pāvutto;",
  "anya-e0602n0:54": "Sokhyaṁ yo nubhavi vimuttiṁtaṁ,",
  "anya-e0602n0:55": "Vande mārāji atulappañño.",
  "anya-e0602n0:56": "19.",
  "anya-e0602n0:57": "Kāruṇiko rājāyatane yo,",
  "anya-e0602n0:58": "Bhoja muḷāraṁ bhojjarasekaṁ;",
  "anya-e0602n0:59": "Vāsa makā nantagguṇadhāro,",
  "anya-e0602n0:60": "Ekagato vandāmi mahesī.",
  "anya-e0602n0:61": "20.",
  "anya-e0602n0:62": "Keyyaṁ keyyaṁ abhijayaketuṁ,",
  "anya-e0602n0:63": "Jeyyaṁ jeyyaṁ varajayapānaṁ;",
  "anya-e0602n0:64": "Peyyaṁ peyyaṁ suvici name taṁ,",
  "anya-e0602n0:65": "Neyyaṁ neyyaṁ samatamupekkhiṁ.",
  "anya-e0602n0:66": "21.",
  "anya-e0602n0:67": "Satta ca sattāhe vijitāvī,",
  "anya-e0602n0:68": "Khepiya nigrodhaṁ puna gantvā;",
  "anya-e0602n0:69": "Dhammasududdasyaṁ pati jāto,",
  "anya-e0602n0:70": "Dhammakathā pposukka vitakko.",
  "anya-e0602n0:71": "22.",
  "anya-e0602n0:72": "Marugaṇehi brahmunā samaṁ,",
  "anya-e0602n0:73": "Ratanadāmameruṇā dadā;",
  "anya-e0602n0:74": "Samabhiyācito paṭissavaṁ,",
  "anya-e0602n0:75": "Dasabalo namāmi desituṁ.",
  "anya-e0602n0:76": "23.",
  "anya-e0602n0:77": "Gantvā isippātakkānanañca,",
  "anya-e0602n0:78": "Satthā migaddāyaṁ desayittha;",
  "anya-e0602n0:79": "Saccappakāraṁ yo dhammacakka-",
  "anya-e0602n0:80": "Suttaṁ name chabbīdhaṁsu tāva.",
  "anya-e0602n0:81": "24.",
  "anya-e0602n0:82": "Rañño magadhassa katappaṭiñño,",
  "anya-e0602n0:83": "Gantvānatha rājagahaṁ vineti;",
  "anya-e0602n0:84": "Satte malarogayute gadaggaṁ,",
  "anya-e0602n0:85": "Pāyetuna taṁ paṇame bhisakkaṁ.",
  "anya-e0602n0:86": "25.",
  "anya-e0602n0:87": "Sakyādhivāse kapile saññatiṁ,",
  "anya-e0602n0:88": "Mānaddhajaṁ bhindiya middhivātā;",
  "anya-e0602n0:89": "Dhammāmataṁ pāyi pituppabhuti,",
  "anya-e0602n0:90": "Vandāmi sakyinda manomadassiṁ.",
  "anya-e0602n0:91": "26.",
  "anya-e0602n0:92": "Anāthapiṇḍoti vhayena nanta-",
  "anya-e0602n0:93": "Ddhanaṁ cajitvā sukate vihāre;",
  "anya-e0602n0:94": "Vihāsi bhiyyo janatā hitatthaṁ,",
  "anya-e0602n0:95": "Sudhammabheriṁ vadayaṁ bhivande.",
  "anya-e0602n0:96": "27.",
  "anya-e0602n0:97": "Upetapuññaṁ varalakkhaṇokaṁ-",
  "anya-e0602n0:98": "Ṇokaṁ visuddhaṁ jalitappadīmaṁ;",
  "anya-e0602n0:99": "Dīpaṁ pajānaṁ anighaṁ navajjaṁ-",
  "anya-e0602n0:100": "Navajjavācaṁ sugataṁ namāmi.",
  "anya-e0602n0:101": "28.",
  "anya-e0602n0:102": "Munibhānu dhammapabhāhi jana-",
  "anya-e0602n0:103": "Mbuja muddhatandhatamo sutapo;",
  "anya-e0602n0:104": "Karuṇāruṇo samabodhayi yo,",
  "anya-e0602n0:105": "Bhavisītalattapano paṇame.",
  "anya-e0602n0:106": "29.",
  "anya-e0602n0:107": "Indu viyambaramajjhatale yo,",
  "anya-e0602n0:108": "Rājati tāragaṇena munindo;",
  "anya-e0602n0:109": "Ariyagaṇapparito tibhave taṁ,",
  "anya-e0602n0:110": "Sādara muggatasobha mavandi.",
  "anya-e0602n0:111": "30.",
  "anya-e0602n0:112": "Mūlāmūlā tiṁsatipāramiyo,",
  "anya-e0602n0:113": "Sākhāsākhā majjhimajjhānakāyā;",
  "anya-e0602n0:114": "Pattāpattā buddhacakkhuni yassa,",
  "anya-e0602n0:115": "Pupphāpupphā kāruṇā maggakhandho.",
  "anya-e0602n0:116": "31.",
  "anya-e0602n0:117": "Sārāsārā yamakā pheggubhiññā,",
  "anya-e0602n0:118": "Sabbālambaṅkurakaṁ sīlavaṇṭaṁ;",
  "anya-e0602n0:119": "Sesādhammā madhupakkapphalāni,",
  "anya-e0602n0:120": "Raṁsīchallī sutacā lakkhaṇāni.",
  "anya-e0602n0:121": "32.",
  "anya-e0602n0:122": "Chāyupagā suṁ puthunaradevā,",
  "anya-e0602n0:123": "Mārajidīpaṅkara mahijātaṁ;",
  "anya-e0602n0:124": "Patthatanantañca kusalabījaṁ,",
  "anya-e0602n0:125": "Ekaruhaṁ pādapamuni vande.",
  "anya-e0602n0:126": "33.",
  "anya-e0602n0:127": "Māratitthiyaripuṁ tamañca yo,",
  "anya-e0602n0:128": "Dhaṁsayaṁ nagaradhamma māpayaṁ;",
  "anya-e0602n0:129": "Sīlanīvaraṇa dvārakoṭṭhakaṁ,",
  "anya-e0602n0:130": "Esikāparamasaddha muttamaṁ.",
  "anya-e0602n0:131": "34.",
  "anya-e0602n0:132": "Dvārapālasuyataṁ satiṭālaṁ,",
  "anya-e0602n0:133": "Ñāṇacaccara sighāṭapadiddhiṁ;",
  "anya-e0602n0:134": "Dhammamaggakuṭisāla sahittaṁ,",
  "anya-e0602n0:135": "Dhammasenapatisārijabhūtaṁ.",
  "anya-e0602n0:136": "35.",
  "anya-e0602n0:137": "Dutiyasutaparohitaṁ mahā-",
  "anya-e0602n0:138": "Dhutaguṇadharamaggha dassikaṁ;",
  "anya-e0602n0:139": "Vinayacinaka dhammarakkhakaṁ,",
  "anya-e0602n0:140": "Tibhavapatika dhammarājiyaṁ.",
  "anya-e0602n0:141": "36.",
  "anya-e0602n0:142": "Sutaṁ sutaṁ sabbadhi sajjanaṁjanaṁ,",
  "anya-e0602n0:143": "Mitaṁ mitaṁ nantaguṇālayaṁ layaṁ;",
  "anya-e0602n0:144": "Hutaṁ hutaṁ jantunamānamaṁ namaṁ,",
  "anya-e0602n0:145": "Bhave bhave tebhavamaṅgalaṁ galaṁ.",
  "anya-e0602n0:146": "37.",
  "anya-e0602n0:147": "Lokābhirāmaṁ ramaṇeyyaveduraṁ,",
  "anya-e0602n0:148": "Lokantaguṁ antubhayānupāgataṁ;",
  "anya-e0602n0:149": "Lokaddhajaṁ mānadhajappahārakaṁ,",
  "anya-e0602n0:150": "Lokaggaruṁ aggaru paccaye name.",
  "anya-e0602n0:151": "38.",
  "anya-e0602n0:152": "Makuṭaggaphalaṁ nayanañcanikaṁ,",
  "anya-e0602n0:153": "Savanabbhusanaṁ vadanabbhusanaṁ;",
  "anya-e0602n0:154": "Galamaṇḍanamiddhi manaññurasaṁ,",
  "anya-e0602n0:155": "Kiriyaññudaraṁ dvikhaṇaññukaraṁ.",
  "anya-e0602n0:156": "39.",
  "anya-e0602n0:157": "Kaṭivibhūsana majjhimaññaṇakaṁ,",
  "anya-e0602n0:158": "Dayavilepana dhāraṇamaṇḍanaṁ;",
  "anya-e0602n0:159": "Paṭakhilaññunivattha manuttaraṁ,",
  "anya-e0602n0:160": "Munimaruttama meka mahaṁ name.",
  "anya-e0602n0:161": "40.",
  "anya-e0602n0:162": "Sativara sucakaṁ ibbhaggapaññaṁ,",
  "anya-e0602n0:163": "Suvīriyavasuvāhaṁ pītiselaṁ;",
  "anya-e0602n0:164": "Gahapatisamathaggaṁ thīpasaddhiṁ,",
  "anya-e0602n0:165": "Asaṇiyasunupekkhaṁ sattabhogaṁ.",
  "anya-e0602n0:166": "41.",
  "anya-e0602n0:167": "Catudisikiddhippada masahāyaṁ,",
  "anya-e0602n0:168": "Suparivutaṁ khattiyavirajānaṁ;",
  "anya-e0602n0:169": "Suciparisaṁ devanara manāpaṁ,",
  "anya-e0602n0:170": "Dasabalacakkādhipati bhivande.",
  "anya-e0602n0:171": "42.",
  "anya-e0602n0:172": "Samādhibbahiddhaṁ saticchaṭṭhaselaṁ,",
  "anya-e0602n0:173": "Virappañcamā jīvamajjhaṁ sukamma-",
  "anya-e0602n0:174": "Ttatiyaṁ suvācādutiyaṁ bhiseṭṭhaṁ,",
  "anya-e0602n0:175": "Samātakkapubbañca sattappavāraṁ.",
  "anya-e0602n0:176": "43.",
  "anya-e0602n0:177": "Vividhañāṇapabhutibbasākaraṁ,",
  "anya-e0602n0:178": "Bahumahiddhika marūna māsayaṁ;",
  "anya-e0602n0:179": "Dhuṁvapabhājalitarāmaṇeyyakaṁ,",
  "anya-e0602n0:180": "Sugatanerunagarāja māname.",
  "anya-e0602n0:181": "44.",
  "anya-e0602n0:182": "Sīlajjalaṁ ratanadhamma mākaraṁ,",
  "anya-e0602n0:183": "Bhiññāviciṁ satikulaṭṭhi gambhiraṁ;",
  "anya-e0602n0:184": "Vitthiṇṇañāṇa mariyodajāsayaṁ,",
  "anya-e0602n0:185": "Aṭṭhambīdhacchariyakaṁ asandanaṁ.",
  "anya-e0602n0:186": "45.",
  "anya-e0602n0:187": "Bahunajjamosaraṇa mekarasaṁ,",
  "anya-e0602n0:188": "Durupaggamaṁ acitasambharinaṁ;",
  "anya-e0602n0:189": "Puthupotapuññavatameva name,",
  "anya-e0602n0:190": "Dvīpaduttamaṇṇava nadindavaraṁ.",
  "anya-e0602n0:191": "46.",
  "anya-e0602n0:192": "Abhiniharadhanuṁ guṇapāramiṁ,",
  "anya-e0602n0:193": "Naḷamatimanabājarahammukhā;",
  "anya-e0602n0:194": "Dhitisamitasarena sudhārayaṁ,",
  "anya-e0602n0:195": "Malama gagaṇa mekapavedanā.",
  "anya-e0602n0:196": "47.",
  "anya-e0602n0:197": "Yo pāvedhesiṁ aggamaggañca tūṇiṁ,",
  "anya-e0602n0:198": "Dhammālaṅkārabbamma mekabbalaṁ taṁ;",
  "anya-e0602n0:199": "Nettiṁsānantañāṇa gīrellimantaṁ,",
  "anya-e0602n0:200": "Sabbaññussāsaṁ iddhidaṇḍaṁ namāmi.",
  "anya-e0602n0:201": "48.",
  "anya-e0602n0:202": "Varatapavuṭṭhi subījakasaddhā,",
  "anya-e0602n0:203": "Matiyuganaṅgala hīrutapīsā;",
  "anya-e0602n0:204": "Susamatharajju satidvijatuttaṁ,",
  "anya-e0602n0:205": "Vīriyadurāvaha saccanidānaṁ.",
  "anya-e0602n0:206": "49.",
  "anya-e0602n0:207": "Vatisaṁvaraṁ soratamocanañcaṁ,",
  "anya-e0602n0:208": "Saraṇā nivattabbahanatthaṁ yassa;",
  "anya-e0602n0:209": "Amatapphalaṁ nekarasehupetaṁ,",
  "anya-e0602n0:210": "Sugataṁ mahākassaka mābhivande.",
  "anya-e0602n0:211": "50.",
  "anya-e0602n0:212": "Devaggo tidasapure varāsane yo,",
  "anya-e0602n0:213": "Devānaṁ janikapabhutinaṁ bhidhammaṁ;",
  "anya-e0602n0:214": "Bālakkova laḷayamācale timāsaṁ,",
  "anya-e0602n0:215": "Desesyā paravisayaṁ name ajeyyo.",
  "anya-e0602n0:216": "51.",
  "anya-e0602n0:217": "Mahāyaso vividhasubhappakāsakaṁ,",
  "anya-e0602n0:218": "Kurūsu yo amitaguṇo tamonudo;",
  "anya-e0602n0:219": "Parappavādahari subhānuyoginaṁ,",
  "anya-e0602n0:220": "Namāmi taṁ kathayi satippaṭhānakaṁ.",
  "anya-e0602n0:221": "52.",
  "anya-e0602n0:222": "Samathakapaliko sasnehasammāsati,",
  "anya-e0602n0:223": "Paramamatiginī savaṭṭiparakkamo;",
  "anya-e0602n0:224": "Sakalajutikaro sudhammapadīpako,",
  "anya-e0602n0:225": "Ima mupajalito jinena namāmahaṁ.",
  "anya-e0602n0:226": "53.",
  "anya-e0602n0:227": "Vigatagatamalaṁ malagatavigataṁ,",
  "anya-e0602n0:228": "Mahita hitamanaṁ manahitamahitaṁ;",
  "anya-e0602n0:229": "Vibhavabhavakaraṁ karabhavavibhavaṁ,",
  "anya-e0602n0:230": "Sujana janaguṇaṁ guṇajanasujanaṁ.",
  "anya-e0602n0:231": "54.",
  "anya-e0602n0:232": "Sīlaggadaṇḍavicitaṁ susamādhipattaṁ,",
  "anya-e0602n0:233": "Sobhāsamujjala manantaga ñāṇasīkhaṁ;",
  "anya-e0602n0:234": "Saddhammaseṭṭharatanañca tilokaketuṁ,",
  "anya-e0602n0:235": "Vandāmi locanabhiseka susobhayuttaṁ.",
  "anya-e0602n0:236": "55.",
  "anya-e0602n0:237": "Vinayanaya manayavinaya manamitaṁ,",
  "anya-e0602n0:238": "Vijayajaya majayavijaya matulitaṁ;",
  "anya-e0602n0:239": "Vibhajabhaja mabhajavibhaja mananakaṁ,",
  "anya-e0602n0:240": "Visamasama masamavisama mabhiname.",
  "anya-e0602n0:241": "56.",
  "anya-e0602n0:242": "Paramarama maramaparama matiguṇaṁ,",
  "anya-e0602n0:243": "Pagahagati magatipagata mamamakaṁ;",
  "anya-e0602n0:244": "Pacayacaya macayapapaya manaṇakaṁ,",
  "anya-e0602n0:245": "Pakatakata makatapakata macalakaṁ.",
  "anya-e0602n0:246": "57.",
  "anya-e0602n0:247": "Ujuka mayanamagge mokkhadesaṁ niyāsi,",
  "anya-e0602n0:248": "Vararathakujarena cammacakkena satthā;",
  "anya-e0602n0:249": "Hiritapadukapālambena dhammassutena,",
  "anya-e0602n0:250": "Satinivarayutena ppāṭihiraddhajena.",
  "anya-e0602n0:251": "58.",
  "anya-e0602n0:252": "Avihananakkhinā suyama nemupakkharā,",
  "anya-e0602n0:253": "Udariyamabbhinā paripuraṅgasaccinā;",
  "anya-e0602n0:254": "Kusalavibhūsinā nimadakupparena yo,",
  "anya-e0602n0:255": "Akharanatesinā gupitasilanandanā.",
  "anya-e0602n0:256": "59.",
  "anya-e0602n0:257": "Anusanughātinā matipurejavena kāla-",
  "anya-e0602n0:258": "Ññutamatisārike na ca visāradatthidaṇḍā;",
  "anya-e0602n0:259": "Satituda dhītirasmi manadammasindhavena,",
  "anya-e0602n0:260": "Vinayagaṇe namāmi ta matulyasatthavāhaṁ.",
  "anya-e0602n0:261": "60.",
  "anya-e0602n0:262": "Yamakkaggijālaṁ paravisaya maccherasahitaṁ,",
  "anya-e0602n0:263": "Dudiṭṭhandhubbāhaṁ yugagahaṇatitthīna makarī;",
  "anya-e0602n0:264": "Bahūnaṁ majjhe yo ratanakamane pāṭihariyaṁ,",
  "anya-e0602n0:265": "Jayakketussāpi ta mabhinami kaṇṭambasamipe.",
  "anya-e0602n0:266": "61.",
  "anya-e0602n0:267": "Nakhajutirajaṁ cakkaṅgopeta pādavarambujaṁ,",
  "anya-e0602n0:268": "Subhasirimato raṁsijālaṅgulidasasaṁsubhiṁ;",
  "anya-e0602n0:269": "Pavarasirasā devādevā sadā na pilandhayuṁ,",
  "anya-e0602n0:270": "Ta mativa manorammaṁ tittīkarā nami yassa ke.",
  "anya-e0602n0:271": "62.",
  "anya-e0602n0:272": "Buddhopyeko nidhanaguṇino vaṇṇaye yāvajīvaṁ,",
  "anya-e0602n0:273": "Kāmaṁ aññaṁ kathamabhaṇa māsuṁ khiyethā yukappo;",
  "anya-e0602n0:274": "Na tvevā yaṁ khaya mupavajje yassa vaṇṇo ananto,",
  "anya-e0602n0:275": "Taṁ sabbaññuṁ sakalarirī nekanāthaṁ namāmi.",
  "anya-e0602n0:276": "63.",
  "anya-e0602n0:277": "Pādidīpādaṁ dvinayanadijaṁ dhammakāyaṁ dhisoṇḍaṁ,",
  "anya-e0602n0:278": "Bhāṇīsoṇḍaggaṁ saraṇasirasiṁ maggāvālaṁ subhaṅgaṁ;",
  "anya-e0602n0:279": "Sīlālaṅkāraṁ vimalibhavu taṁ sattatiṭṭhidhidabbaṁ,",
  "anya-e0602n0:280": "Nāthebhindaggaṁ phalakariṇukaṁ mokkhabhojaṁ namāmi.",
  "anya-e0602n0:281": "64.",
  "anya-e0602n0:282": "Malāloḷullolaṁ atibhayajanaṁ duggasaṁsārasinduṁ,",
  "anya-e0602n0:283": "Phiyabbhāniggāho sivataṭamukho nāvi kajjeṭṭhanātho;",
  "anya-e0602n0:284": "Padappārakkāmaṁ bahujanagaṇaṁ ekamaggattaramhi,",
  "anya-e0602n0:285": "Samāropetvā mattari ta matulasādarañcā bhivande.",
  "anya-e0602n0:286": "65.",
  "anya-e0602n0:287": "Byāmaṁsugghanadhāra makkuḷusatabbhāṇujjalantattanaṁ,",
  "anya-e0602n0:288": "Ukkaṁsajjuti ketumālavicitaṁ saddhammajotindharaṁ;",
  "anya-e0602n0:289": "Bundinniggatapajjalanta didhitiṁ ajjatthanā yāva ca,",
  "anya-e0602n0:290": "Vandetaṁ muni sakyapuṅgava mahaṁ puṇṇinduvattampi ca.",
  "anya-e0602n0:291": "66.",
  "anya-e0602n0:292": "Sattamaṁtamaṁ vināsakaṁsakaṁ dadaṁ vineyyakānameva,",
  "anya-e0602n0:293": "Bhāvanaṁvanaṁ dhulīkaraṁ karaṁ tidukkaraṁ pajābhibhuñja;",
  "anya-e0602n0:294": "Gāravaṁravaṁ manoharaṁharaṁ narānarānayaṁ namāmi,",
  "anya-e0602n0:295": "Sādaraṁdaraṁ vinodakaṁdakaṁ pavassakaṁ pajānamiva.",
  "anya-e0602n0:296": "67.",
  "anya-e0602n0:297": "Buddho nigrodhabimbo mudukaracaraṇo brahmaghoseṇijaṅgho,",
  "anya-e0602n0:298": "Kosacchādaṅgajāto punarapi sugato suppatiṭṭhitapādo;",
  "anya-e0602n0:299": "Mudodātuṇṇalomo athamapi sugato brahmujuggattabhāvo,",
  "anya-e0602n0:300": "Nīlakkhī dīghapaṇhī sukhumamalachavī thomyarassaggasaggī.",
  "anya-e0602n0:301": "68.",
  "anya-e0602n0:302": "Cattālīsaggadanto samakalapanajo antaraṁsappapīṇo,",
  "anya-e0602n0:303": "Cakkenaṅkītapādo aviraḷadasano mārajussaṅkhapādo;",
  "anya-e0602n0:304": "Tiṭṭhantonomento bhayakaramudunā jaṇṇukānā masanto,",
  "anya-e0602n0:305": "Vaṭṭakkhandho jino gotaruṇapakhumako sīhapubbaḍḍhukāyo.",
  "anya-e0602n0:306": "69.",
  "anya-e0602n0:307": "Sattappīṇo ca dīghaṅgulaṁ matha sugato lomakūpekalomo,",
  "anya-e0602n0:308": "Sampannodātadāṭho kanakasamataco nīlamuddhaggalomo;",
  "anya-e0602n0:309": "Sambuddho thūlajivho atha sīhahanuko jālikappādahattho,",
  "anya-e0602n0:310": "Nātho uṇhīsasīso iti guṇasahitaṁ taṁ mahesiṁ namāmi.",
  "anya-e0602n0:311": "70.",
  "anya-e0602n0:312": "Vaṭṭacitānupubbakasubhaṅgulī ruhiramaṭṭhatuṅganakhako,",
  "anya-e0602n0:313": "Nigguḷagopphako samapado sīhosabhibha haṁsasannibhakamo;",
  "anya-e0602n0:314": "Dakkhiṇatāvatakkami samantamaṇḍala nigaṇṭhi jāṇusubhako,",
  "anya-e0602n0:315": "Byañjanapuṇṇaposatanu nābhigambhīra achiddadakkhiṇavaṭo.",
  "anya-e0602n0:316": "71.",
  "anya-e0602n0:317": "Dviradakarappakāsurubhujo suvibbhajanupubbamaṭṭhaanunā-",
  "anya-e0602n0:318": "Nunaalinānupubbarucira ttilādirahitabbisuddhatanuko;",
  "anya-e0602n0:319": "Dasasatakoṭi hatthibaladhāraṇo kanakatuṅganāsikasubho,",
  "anya-e0602n0:320": "Suruhiradantamaṁsatha sucisiniddhadasano tha lokasaraṇo.",
  "anya-e0602n0:321": "72.",
  "anya-e0602n0:322": "Suddhapasādindri ca vaṭṭataradāṭho ruhiroṭṭha ca suranaranātho,",
  "anya-e0602n0:323": "Āyatasobhabbadano tha muni gambhīrujukāyatasuruciralekho;",
  "anya-e0602n0:324": "Byāmapabhāmaṇḍalabundi supuraggaṇṇi ca āyatavisaṭasubhakkhī,",
  "anya-e0602n0:325": "Pañcapasādakkhi ca kuñcikasubhaggapakhumo mudutanuruṇajivho.",
  "anya-e0602n0:326": "73.",
  "anya-e0602n0:327": "Sommasiniddha tyujjalakomala varuṇavimalatanu ca amitaguṇo,",
  "anya-e0602n0:328": "Komala dakkhiṇāvaṭa añjanabhidasarisanilaka mudutanuruho;",
  "anya-e0602n0:329": "Dakkhiṇavaṭṭakomala saṇusamasunila alulita siraruhi jino,",
  "anya-e0602n0:330": "Sobhaṇasaṇṭhāno tha siniddhasiraruhi ca supacitasatakusala jo.",
  "anya-e0602n0:331": "74.",
  "anya-e0602n0:332": "Nigguḷonigganticchattassarisaatisubhagasira cāyatāruci kaṇṇako,",
  "anya-e0602n0:333": "Sosaṇṭhānassaṇhāhārānukamapahutabhamutha suāyatabbhamuko ca so;",
  "anya-e0602n0:334": "Suggandhaggatto muddhicātha vadani ca puthulakanalāṭa āyatasobhaṇo,",
  "anya-e0602n0:335": "Assāsappassāsātissaṇu dharamasamasama nami ketumālavicittakaṁ.",
  "anya-e0602n0:336": "75.",
  "anya-e0602n0:337": "Buddhuppādo kimaṅgaṁ atidulabhataraṁ ghosamattampi loke,",
  "anya-e0602n0:338": "Tasmā nānappakāraṁ saparahitasukhaṁ viddhasū patthayantā;",
  "anya-e0602n0:339": "Yātiṭṭhatthāva haṁ taṁ suranarasaraṇaṁ antarāyappahānaṁ,",
  "anya-e0602n0:340": "Puññakkhettekabhūtaṁ sugatamavirataṁ sādhu vandantu santo.",
  "anya-e0602n0:341": "76.",
  "anya-e0602n0:342": "Khettavaraṅgatatthutipure javapaṇama tejasā idha bhave,",
  "anya-e0602n0:343": "Rogabhayādyupaddavahato anunasukha bhogapuññamatiko;",
  "anya-e0602n0:344": "Devamanussabhogapavaraṁ paratthanubhāvañca antimabhave,",
  "anya-e0602n0:345": "Aññataro tibodhipavare bhavissati yathāsayaṁ katanato.",
  "anya-e0602n0:346": "77.",
  "anya-e0602n0:347": "Puññenānena sohaṁ nipuṇajavamati pemavāco sakhīlo,",
  "anya-e0602n0:348": "Saddho kalyāṇamittotisaraṇagamano sīlavā cāgayogo;",
  "anya-e0602n0:349": "Hirottappī sudakkho avitasucarito dhitimā saccabhāṇī,",
  "anya-e0602n0:350": "Bāhussacci vibhāgi saparahitakaro vaggurāvo bhirupo.",
  "anya-e0602n0:351": "78.",
  "anya-e0602n0:352": "Dīghajjīvi nirogo sucikulapassuto dhammaratto viratto,",
  "anya-e0602n0:353": "Niccāpalyo kataññu atimudujumano sādhubhāvādiviññū;",
  "anya-e0602n0:354": "Dhammājīvo bhaveyyaṁ bahukusalarato appakodho aluddho,",
  "anya-e0602n0:355": "Evañcaññaṁ kareyyaṁ paṇidhi carimake mokkhanibbānabhāgī.",
  "anya-e0602n0:356": "79.",
  "anya-e0602n0:357": "(1) Mahākathaṁ buddhaghoso, tanumeva karaṁapi;",
  "anya-e0602n0:358": "Cajaṁ heyyā diyā deyyaṁ, akarittha yathā tathā.",
  "anya-e0602n0:359": "80.",
  "anya-e0602n0:360": "(2) Mahāpaṇāmaporāṇaṁ, kiñci eva punappunaṁ;",
  "anya-e0602n0:361": "Kāmokkamaṁ dudhārañca, cajaṁ deyyā diyaññattha.",
  "anya-e0602n0:362": "81.",
  "anya-e0602n0:363": "(3) Sutajjaya anubhava-ṭṭhapanatthena lañchinā;",
  "anya-e0602n0:364": "Sutena garunānena, katoyaṁ paṇāmo navo.",
  "anya-e0602n0:365": "82.",
  "anya-e0602n0:366": "(4) Ekakkharāya gāthāyo, yāva chabbisatakkharā;",
  "anya-e0602n0:367": "Jātiya pajjasattatyā, saṅkhato caturādhikā.",
  "anya-e0602n0:368": "83.",
  "anya-e0602n0:369": "(5) Aṭṭhādhikā sahevuyyo- janādīhi midaṁ nataṁ;",
  "anya-e0602n0:370": "Yathāvuttatthakā kāmā, ye niccaṁ dhārayantu te.",
  "anya-e0602n0:371": "84.",
  "anya-e0602n0:372": "Rājātirājātimanoharo yo,",
  "anya-e0602n0:373": "Devātidevātiguṇoghadhārī;",
  "anya-e0602n0:374": "Brahmātibrahmātibhavantagū taṁ,",
  "anya-e0602n0:375": "Saṅghātisaṅghātivirāva vande.",
  "anya-e0602n0:376": "85.",
  "anya-e0602n0:377": "Anaṅganaṅgaṁ naradevadevaṁ,",
  "anya-e0602n0:378": "Aniñjaniñjaṁ bhayatāṇatāṇaṁ;",
  "anya-e0602n0:379": "Anaṇḍanaṇḍañca anāthanāthaṁ,",
  "anya-e0602n0:380": "Khayantayantaṁ paṇamāmi māmi.",
  "anya-e0602n0:381": "86.",
  "anya-e0602n0:382": "Tambasiniddhatuṅganakhako nuṁvaṭṭasucituraṅguli ca muniso,",
  "anya-e0602n0:383": "Sīhusabhobhahaṁsasamago nigūḷasama gopphakāyatamukho;",
  "anya-e0602n0:384": "Komaladakkhāṇāvaṭatanuruho sucimalujjalābhasariro,",
  "anya-e0602n0:385": "Pañcapasādayuttanayano sugandhamukhatuṅganāsikayuto.",
  "anya-e0602n0:386": "87.",
  "anya-e0602n0:387": "Koṭisahassanāgabaliko surattamadharo suvaṭṭadasano,",
  "anya-e0602n0:388": "Āyatasaṇulomabhamūko muduttanukaratta jivhasahito;",
  "anya-e0602n0:389": "Chattasamānasobhaṇasiro sukesavara ketumālavicito,",
  "anya-e0602n0:390": "Iccanubyañjanebhi sahitaṁ munindapavaraṁ namāmi sirasā.",
  "anya-e0602n0:391": "88.",
  "anya-e0602n0:392": "Sakalamalehi so muni sudūratāya ca malāriniṁ hatatayā,",
  "anya-e0602n0:393": "Tibhavarathe samānitamanārakāni ca namālayo naravaro;",
  "anya-e0602n0:394": "Malakaraṇe rahāraha manantañeyya mabhijānanā muni tathā,",
  "anya-e0602n0:395": "Caraṇayuto tivijji ca suvācatā sugamanā janesu sugato.",
  "anya-e0602n0:396": "89.",
  "anya-e0602n0:397": "Lokavidū so nitalokatayatā sākalato asamanaradamā so,",
  "anya-e0602n0:398": "Sārathi jino anusāsanakaro satthavaho dupathataraṇasattā;",
  "anya-e0602n0:399": "Bujjhati sāmaṁ catusaccamakhilaṁ bodhayi jantugaṇamiti ca buddho,",
  "anya-e0602n0:400": "Bhākaraābhāphuṭapaṅkajasamo maggiyañāṇaphuvikasito ca.",
  "anya-e0602n0:401": "90.",
  "anya-e0602n0:402": "Bhaggakileso so bhagavā tibhava vamita gamana sujana bhajanato,",
  "anya-e0602n0:403": "So bhaji saddhamme pavibhatta sarasa chabhagayuta garukaraṇiyato;",
  "anya-e0602n0:404": "Sattanikāye kenapitulyaguṇamapamita sirighaṁnajutisusubhaṁ,",
  "anya-e0602n0:405": "Devanarānaṁ ekapatiṭṭha mavitathutiyasa masaki mabhaṁname.",
  "anya-e0602n0:406": "91.",
  "anya-e0602n0:407": "Buddhuppādo kimmaṅgaṁ bho atidulabhataramidha bhave sughosa mapāparo,",
  "anya-e0602n0:408": "Tasmā patthentā sabbaññuṁ vividhahitasukha manadhikaṁ namantu ca sādhavo;",
  "anya-e0602n0:409": "Puññenānenete diṭṭhe bhayaaghaṁpīḷanaṭṭha virahitā parattha cubho subhe,",
  "anya-e0602n0:410": "Bhutvānante ve hessante avikalasukhasirimatikā anuttara bhāgino.",
  "anya-e0602n0:411": "92.",
  "anya-e0602n0:412": "So cakkopetapādo mudubhujacaraṇo suppatiṭṭhitapādo,",
  "anya-e0602n0:413": "Eṇījaṅgho ca buddho kanakanibhataco āyatapaṇhi nātho;",
  "anya-e0602n0:414": "Kosonaddhaṅgajāto atisudhumachavī jālikappānaheṭṭhā,",
  "anya-e0602n0:415": "Ussaṅkhapādayutto abhinilanayano āyataṅguliyogo.",
  "anya-e0602n0:416": "93.",
  "anya-e0602n0:417": "Ṭhito kho no namanto kirubhayaputhunājāṇuyo āmasanto,",
  "anya-e0602n0:418": "Lomakūpekekalomo samataladasano añjanuddhaggalomo;",
  "anya-e0602n0:419": "Brahmaddehujjugatto aviraḷamukhato sattakaṅgussado so,",
  "anya-e0602n0:420": "Nigrodhappāribimbo migapatihanuko sīhapubbaḍḍhakāyo.",
  "anya-e0602n0:421": "94.",
  "anya-e0602n0:422": "Puṇṇattālīsadanto supahutarasano sobhaṇodātadāṭho,",
  "anya-e0602n0:423": "Saṇhodātuṇṇalomo samavaṭalagalo antaraṁsapiṇo so;",
  "anya-e0602n0:424": "Brahmagghoso munindo punapi gupakhumo uṇhisasamphullasīso,",
  "anya-e0602n0:425": "Bāttiṁsaṅgopasobhaṁ mudurasahaṇī lokajeṭṭhaṁ name taṁ.",
  "anya-e0602n0:426": "Mahāpaṇāma niṭṭhitā."
}