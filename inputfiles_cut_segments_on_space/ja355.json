{
  "ja355:0.1": "Jātaka ",
  "ja355:0.2": "Pañcakanipāta ",
  "ja355:0.3": "Maṇikuṇḍalavagga ",
  "ja355:0.4": "5. Ghaṭajātaka ",
  "ja355:1.1": "“Aññe socanti rodanti, ",
  "ja355:1.2": "aññe assumukhā janā; ",
  "ja355:1.3": "Pasannamukhavaṇṇosi, ",
  "ja355:1.4": "kasmā ghaṭa na socasi”. ",
  "ja355:2.1": "“Nābbhatītaharo soko, ",
  "ja355:2.2": "nānāgatasukhāvaho; ",
  "ja355:2.3": "Tasmā dhaṅka na socāmi, ",
  "ja355:2.4": "natthi soke dutīyatā. ",
  "ja355:3.1": "Socaṁ paṇḍu kiso hoti, ",
  "ja355:3.2": "bhattañcassa na ruccati; ",
  "ja355:3.3": "Amittā sumanā honti, ",
  "ja355:3.4": "sallaviddhassa ruppato. ",
  "ja355:4.1": "Gāme vā yadi vāraññe, ",
  "ja355:4.2": "ninne vā yadi vā thale; ",
  "ja355:4.3": "Ṭhitaṁ maṁ nāgamissati, ",
  "ja355:4.4": "evaṁ diṭṭhapado ahaṁ. ",
  "ja355:5.1": "Yassattā nālamekova, ",
  "ja355:5.2": "sabbakāmarasāharo; ",
  "ja355:5.3": "Sabbāpi pathavī tassa, ",
  "ja355:5.4": "na sukhaṁ āvahissatī”ti. ",
  "ja355:6.1": "Ghaṭajātakaṁ pañcamaṁ. "
}