{
  "ja82:0.1": "Jātaka ",
  "ja82:0.2": "Ekakanipāta ",
  "ja82:0.3": "Apāyimhavagga ",
  "ja82:0.4": "2. Mittavindakajātaka ",
  "ja82:1.1": "“Atikkamma ramaṇakaṁ, ",
  "ja82:1.2": "sadāmattañca dūbhakaṁ; ",
  "ja82:1.3": "Svāsi pāsāṇamāsīno, ",
  "ja82:1.4": "yasmā jīvaṁ na mokkhasī”ti. ",
  "ja82:2.1": "Mittavindakajātakaṁ dutiyaṁ. "
}