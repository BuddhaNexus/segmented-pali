{
  "ja342:0.1": "Jātaka ",
  "ja342:0.2": "Catukkanipāta ",
  "ja342:0.3": "Cūḷakuṇālavagga ",
  "ja342:0.4": "2. Vānarajātaka ",
  "ja342:1.1": "“Asakkhiṁ vata attānaṁ, ",
  "ja342:1.2": "uddhātuṁ udakā thalaṁ; ",
  "ja342:1.3": "Na dānāhaṁ puna tuyhaṁ, ",
  "ja342:1.4": "vasaṁ gacchāmi vārija. ",
  "ja342:2.1": "Alametehi ambehi, ",
  "ja342:2.2": "jambūhi panasehi ca; ",
  "ja342:2.3": "Yāni pāraṁ samuddassa, ",
  "ja342:2.4": "varaṁ mayhaṁ udumbaro. ",
  "ja342:3.1": "Yo ca uppatitaṁ atthaṁ, ",
  "ja342:3.2": "na khippamanubujjhati; ",
  "ja342:3.3": "Amittavasamanveti, ",
  "ja342:3.4": "pacchā ca anutappati. ",
  "ja342:4.1": "Yo ca uppatitaṁ atthaṁ, ",
  "ja342:4.2": "khippameva nibodhati; ",
  "ja342:4.3": "Muccate sattusambādhā, ",
  "ja342:4.4": "na ca pacchānutappatī”ti. ",
  "ja342:5.1": "Vānarajātakaṁ dutiyaṁ. "
}