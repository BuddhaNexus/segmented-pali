{
  "ja148:0.1": "Jātaka ",
  "ja148:0.2": "Ekakanipāta ",
  "ja148:0.3": "Kakaṇṭakavagga ",
  "ja148:0.4": "8. Siṅgālajātaka ",
  "ja148:1.1": "“Nāhaṁ punaṁ na ca punaṁ, ",
  "ja148:1.2": "na cāpi apunappunaṁ; ",
  "ja148:1.3": "Hatthibondiṁ pavekkhāmi, ",
  "ja148:1.4": "tathā hi bhayatajjito”ti. ",
  "ja148:2.1": "Siṅgālajātakaṁ aṭṭhamaṁ. "
}