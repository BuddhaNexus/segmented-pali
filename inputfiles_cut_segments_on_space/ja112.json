{
  "ja112:0.1": "Jātaka ",
  "ja112:0.2": "Ekakanipāta ",
  "ja112:0.3": "Haṁcivagga ",
  "ja112:0.4": "2. Amarādevīpañhajātaka ",
  "ja112:1.1": "“Yena sattubilaṅgā ca, ",
  "ja112:1.2": "Diguṇapalāso ca pupphito; ",
  "ja112:1.3": "Yena dadāmi tena vadāmi, ",
  "ja112:1.4": "Yena na dadāmi na tena vadāmi; ",
  "ja112:1.5": "Esa maggo yavamajjhakassa, ",
  "ja112:1.6": "Etaṁ channapathaṁ vijānāhī”ti. ",
  "ja112:2.1": "Amarādevīpañhajātakaṁ dutiyaṁ. "
}