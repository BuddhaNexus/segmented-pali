{
  "tha-ap264:0.1": "Therāpadāna ",
  "tha-ap264:0.2": "Padumukkhipavagga ",
  "tha-ap264:0.3": "2. Telamakkhiyattheraapadāna ",
  "tha-ap264:1.1": "“Siddhatthamhi bhagavati, ",
  "tha-ap264:1.2": "nibbutamhi narāsabhe; ",
  "tha-ap264:1.3": "Bodhiyā vedikāyāhaṁ, ",
  "tha-ap264:1.4": "telaṁ makkhesi tāvade. ",
  "tha-ap264:2.1": "Catunnavutito kappe, ",
  "tha-ap264:2.2": "yaṁ telaṁ makkhayiṁ tadā; ",
  "tha-ap264:2.3": "Duggatiṁ nābhijānāmi, ",
  "tha-ap264:2.4": "makkhanāya idaṁ phalaṁ. ",
  "tha-ap264:3.1": "Catuvīse ito kappe, ",
  "tha-ap264:3.2": "succhavi nāma khattiyo; ",
  "tha-ap264:3.3": "Sattaratanasampanno, ",
  "tha-ap264:3.4": "cakkavattī mahabbalo. ",
  "tha-ap264:4.1": "Paṭisambhidā catasso, ",
  "tha-ap264:4.2": "…pe… ",
  "tha-ap264:4.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap264:5.1": "Itthaṁ sudaṁ āyasmā telamakkhiyo thero imā gāthāyo abhāsitthāti. ",
  "tha-ap264:6.1": "Telamakkhiyattherassāpadānaṁ dutiyaṁ. "
}