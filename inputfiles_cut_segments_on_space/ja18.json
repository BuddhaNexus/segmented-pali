{
  "ja18:0.1": "Jātaka ",
  "ja18:0.2": "Ekakanipāta ",
  "ja18:0.3": "Sīlavagga ",
  "ja18:0.4": "8. Matakabhattajātaka ",
  "ja18:1.1": "“Evañce sattā jāneyyuṁ, ",
  "ja18:1.2": "dukkhāyaṁ jātisambhavo; ",
  "ja18:1.3": "Na pāṇo pāṇinaṁ haññe, ",
  "ja18:1.4": "pāṇaghātī hi socatī”ti. ",
  "ja18:2.1": "Matakabhattajātakaṁ aṭṭhamaṁ. "
}