{
  "ja251:0.1": "Jātaka ",
  "ja251:0.2": "Tikanipāta ",
  "ja251:0.3": "Saṅkappavagga ",
  "ja251:0.4": "1. Saṅkapparāgajātaka ",
  "ja251:1.1": "“Saṅkapparāgadhotena, ",
  "ja251:1.2": "vitakkanisitena ca; ",
  "ja251:1.3": "Nālaṅkatena bhadrena, ",
  "ja251:1.4": "usukārākatena ca. ",
  "ja251:2.1": "Na kaṇṇāyatamuttena, ",
  "ja251:2.2": "nāpi morūpasevinā; ",
  "ja251:2.3": "Tenamhi hadaye viddho, ",
  "ja251:2.4": "sabbaṅgaparidāhinā. ",
  "ja251:3.1": "Āvedhañca na passāmi, ",
  "ja251:3.2": "Yato ruhiramassave; ",
  "ja251:3.3": "Yāva ayoniso cittaṁ, ",
  "ja251:3.4": "Sayaṁ me dukkhamābhatan”ti. ",
  "ja251:4.1": "Saṅkapparāgajātakaṁ paṭhamaṁ. "
}