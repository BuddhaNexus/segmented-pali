{
  "anya-e1007n:6": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1007n:7": "Sūrassatīnīti",
  "anya-e1007n:8": "Paṭhamo bhāgo",
  "anya-e1007n:9": "Paṇāmagāthā",
  "anya-e1007n:10": "1.",
  "anya-e1007n:11": "Mukhamhā bhagavantassa,",
  "anya-e1007n:12": "Sugandhakamalā subhā;",
  "anya-e1007n:13": "Sañjātaṃ uttamaṃ vāṇiṃ,",
  "anya-e1007n:14": "Vandāmi vittamānasā.",
  "anya-e1007n:15": "2.",
  "anya-e1007n:16": "Samajjhiṭṭho racissāmi,",
  "anya-e1007n:17": "Dakkhena rājamantinā;",
  "anya-e1007n:18": "Nāmena santena,",
  "anya-e1007n:19": "Nītiṃ lokahitāvahaṃ;",
  "anya-e1007n:20": "Sālaṅkāraṃ sopadesaṃ,",
  "anya-e1007n:21": "Nāmenāhaṃ svarassatiṃ.",
  "anya-e1007n:22": "3.",
  "anya-e1007n:23": "Pekkhantvimaṃ nītiṃ santā,",
  "anya-e1007n:24": "Mañjūsarukkhasannibhaṃ;",
  "anya-e1007n:25": "Nānopadesasaṃpuṇṇaṃ,",
  "anya-e1007n:26": "Sadatthakusalā sadā.",
  "anya-e1007n:27": "Sūrassatīnīti",
  "anya-e1007n:28": "1.",
  "anya-e1007n:29": "Kataññutā ca saccañca,",
  "anya-e1007n:30": "Lokasārā hi te duve;",
  "anya-e1007n:31": "Lokāpi tehi tiṭṭhanti,",
  "anya-e1007n:32": "Raṭṭhaṃ akaṃsu issaraṃ.",
  "anya-e1007n:33": "2.",
  "anya-e1007n:34": "Kākāca dujjanā loke,",
  "anya-e1007n:35": "Malībhūtāva sabbadā;",
  "anya-e1007n:36": "Iṭṭhaṃ guṇaṃ nāsayanti,",
  "anya-e1007n:37": "Te ve lokassa verino.",
  "anya-e1007n:38": "Migānaṃ siṅgālo anto,",
  "anya-e1007n:39": "Pakkhīnaṃ pana vāyaso.",
  "anya-e1007n:40": "Akāraṇaverī honti,",
  "anya-e1007n:41": "Macchānaṃ dhīvarā yathā;",
  "anya-e1007n:42": "Guṇīnaṃ sajjanānañca,",
  "anya-e1007n:43": "Dujjanā niccaverino.",
  "anya-e1007n:44": "3.",
  "anya-e1007n:45": "Siriṃ bhonto suposetha,",
  "anya-e1007n:46": "Siri mūlā hi sampadā;",
  "anya-e1007n:47": "Siriya idha jotanti,",
  "anya-e1007n:48": "Sirī sā sabbasiddhikā.",
  "anya-e1007n:49": "4.",
  "anya-e1007n:50": "Musā tamokarā loke,",
  "anya-e1007n:51": "Saccaṃ mīditikārakaṃ;",
  "anya-e1007n:52": "Musātamena dukkhanti,",
  "anya-e1007n:53": "Saccābhāya sukhantive.",
  "anya-e1007n:54": "Suvijānaṃ siṅgālānaṃ,",
  "anya-e1007n:55": "Sakuṇānañca vassitaṃ;",
  "anya-e1007n:56": "Manussavassitaṃ rāja,",
  "anya-e1007n:57": "Dubbijānataraṃ tato.",
  "anya-e1007n:58": "5.",
  "anya-e1007n:59": "Kappaggisadisā issā,",
  "anya-e1007n:60": "Jhāpeti sabbasampadaṃ;",
  "anya-e1007n:61": "Muditā kappameghova,",
  "anya-e1007n:62": "Ropeti sabbasampadaṃ.",
  "anya-e1007n:63": "6.",
  "anya-e1007n:64": "Yathā asanthirā thambhā,",
  "anya-e1007n:65": "Thusarāsimhi ussitā;",
  "anya-e1007n:66": "Tatheva kapicittānaṃ,",
  "anya-e1007n:67": "Kammantā cañcalaṅgatā.",
  "anya-e1007n:68": "Haliddirāgaṃ kapicittaṃ;",
  "anya-e1007n:69": "Purisaṃ rāgavirāginaṃ,",
  "anya-e1007n:70": "Edisaṃ tāta māsevi;",
  "anya-e1007n:71": "Nimmanussampi ce siyā.",
  "anya-e1007n:72": "7. [Ka]",
  "anya-e1007n:73": "Pupphacumbi cittapattī,",
  "anya-e1007n:74": "Sakavaṇṇena majjito;",
  "anya-e1007n:75": "Vikkamī pupphato pupphaṃ,",
  "anya-e1007n:76": "Nassāgāraṃ na saṅgamo.",
  "anya-e1007n:77": "[Kha]",
  "anya-e1007n:78": "Kippilikā dubbaṇṇāpi,",
  "anya-e1007n:79": "Samaggāca parakkamā;",
  "anya-e1007n:80": "Mā hotha pupphacumbīva,",
  "anya-e1007n:81": "Hotha vo pacikā yathā.",
  "anya-e1007n:82": "1.",
  "anya-e1007n:83": "Ye maranti kīḷantā te,",
  "anya-e1007n:84": "Svānā aññoññamoditā;",
  "anya-e1007n:85": "Disvāna chaṭṭitaṃ bhattaṃ,",
  "anya-e1007n:86": "Sīghaverī vihiṃsare.",
  "anya-e1007n:87": "2.",
  "anya-e1007n:88": "Tathekepi janā dāni,",
  "anya-e1007n:89": "Samaggā ññoññamoditā;",
  "anya-e1007n:90": "Dhanahetu vihiṃsanti,",
  "anya-e1007n:91": "Dhīratthu sīghaverikā.",
  "anya-e1007n:92": "9.",
  "anya-e1007n:93": "Vasante hemante gimhe;",
  "anya-e1007n:94": "Neva tālā visesino;",
  "anya-e1007n:95": "Thiracittā janā santā;",
  "anya-e1007n:96": "Sukhadukkhesu niccalā.",
  "anya-e1007n:97": "10.",
  "anya-e1007n:98": "Yathā pavaṭṭamānamhi,",
  "anya-e1007n:99": "Suṭṭhu tiṭṭhati geṇṭhuke;",
  "anya-e1007n:100": "Appavaṭṭe bhūmyaṃ seti,",
  "anya-e1007n:101": "Tatheva geṇṭhuko jano.",
  "anya-e1007n:102": "11.",
  "anya-e1007n:103": "Agghāpetuṃ nasakkonti,",
  "anya-e1007n:104": "Kālañhi kālikā janā;",
  "anya-e1007n:105": "Vajirādiñca sakkonti,",
  "anya-e1007n:106": "Tena kālo anagghiko.",
  "anya-e1007n:107": "12. [Ka]",
  "anya-e1007n:108": "Asano hi dīghaddhāno,",
  "anya-e1007n:109": "Sārasāro sugandhiko;",
  "anya-e1007n:110": "Niggandho tveva nissāro,",
  "anya-e1007n:111": "Dīghaddhānopi simbalī.",
  "anya-e1007n:112": "[Kha]",
  "anya-e1007n:113": "Tatheveke janā loke,",
  "anya-e1007n:114": "Dīghaddhānā susārakā;",
  "anya-e1007n:115": "Nissārā keci pheggūva,",
  "anya-e1007n:116": "Dīghaddhānāpi goyathā.",
  "anya-e1007n:117": "13.",
  "anya-e1007n:118": "Upakāro cāpakāro,",
  "anya-e1007n:119": "Yasmiṃ gacchati naṭṭhataṃ;",
  "anya-e1007n:120": "Pāsāṇahadayassassa,",
  "anya-e1007n:121": "Jīvatītyā bhidhāmudhā.",
  "anya-e1007n:122": "Pasādo nipphalo yassa,",
  "anya-e1007n:123": "Kopocāpi niratthako;",
  "anya-e1007n:124": "Na taṃ saṅgantu miccheyya,",
  "anya-e1007n:125": "Thīpumāva napuṃsakaṃ.",
  "anya-e1007n:126": "14.",
  "anya-e1007n:127": "Upacāro hi kātabbo,",
  "anya-e1007n:128": "Na yāva sohadaṃ bhave;",
  "anya-e1007n:129": "Upacāro sumittamhi,",
  "anya-e1007n:130": "Māyā ca hoti koṭilaṃ.",
  "anya-e1007n:131": "15.",
  "anya-e1007n:132": "Kamena aggato ucchu,",
  "anya-e1007n:133": "Raso sādutaro yathā;",
  "anya-e1007n:134": "Tatheva sumitto loke,",
  "anya-e1007n:135": "Dummitto pana nediso.",
  "anya-e1007n:136": "16.",
  "anya-e1007n:137": "Sokārāti parittāṇaṃ,",
  "anya-e1007n:138": "Vissāsapītibhājanaṃ;",
  "anya-e1007n:139": "Ratanābhiratanaṃ icche,",
  "anya-e1007n:140": "Sumittaṃ akkharattayaṃ.",
  "anya-e1007n:141": "17.",
  "anya-e1007n:142": "Dampatīnaṃ sumittānaṃ,",
  "anya-e1007n:143": "Mukhaṃ aññoññadappaṇaṃ,",
  "anya-e1007n:144": "Sukhe sukhaṃ dukkhe dukkhaṃ,",
  "anya-e1007n:145": "Paṭicchāyeva dappaṇe.",
  "anya-e1007n:146": "Kathaṃ nu tāsaṃ hadayaṃ,",
  "anya-e1007n:147": "Sukharāvata itthiyo;",
  "anya-e1007n:148": "Yāsāmike dukkhitamhi,",
  "anya-e1007n:149": "Sukhamicchanti attano.",
  "anya-e1007n:150": "18.",
  "anya-e1007n:151": "Nivātañca pure katvā,",
  "anya-e1007n:152": "Mānaṃ katvāna pacchato;",
  "anya-e1007n:153": "Sakatthaṃ dhāraye dhīro,",
  "anya-e1007n:154": "Atthabhañjo hi muḷhatā.",
  "anya-e1007n:155": "19.",
  "anya-e1007n:156": "Purecāraṃ satiṃ katvā,",
  "anya-e1007n:157": "Saddhaṃ kareyya pacchato;",
  "anya-e1007n:158": "Turaṃ na saddahe dhīro,",
  "anya-e1007n:159": "Sīghasaddho hi mandako.",
  "anya-e1007n:160": "20.",
  "anya-e1007n:161": "Vane 0.0099 bahūni kaṭṭhāni;",
  "anya-e1007n:162": "Dullabhaṃ rattacandanaṃ;",
  "anya-e1007n:163": "Tathā janā bahū loke;",
  "anya-e1007n:164": "Pumā jañño sudullabho.",
  "anya-e1007n:165": "21.",
  "anya-e1007n:166": "Tiṇakaṭṭhapalāsehi;",
  "anya-e1007n:167": "Sukkhehi dayhate vanaṃ;",
  "anya-e1007n:168": "Etādīhi asārehi;",
  "anya-e1007n:169": "Loko janehi dayhate.",
  "anya-e1007n:170": "22.",
  "anya-e1007n:171": "Antovasse timāsamhi,",
  "anya-e1007n:172": "Puññakammena moditā;",
  "anya-e1007n:173": "Sukhaṃ vasiṃsu porāṇā,",
  "anya-e1007n:174": "Buddhasāsanamāmakā.",
  "anya-e1007n:175": "23.",
  "anya-e1007n:176": "Migamadena ekena,",
  "anya-e1007n:177": "Taṃ vanaṃ surabhigandhikaṃ;",
  "anya-e1007n:178": "Tathā janena taṃ raṭṭhaṃ,",
  "anya-e1007n:179": "Guṇinā hi sirīmatā.",
  "anya-e1007n:180": "Sarīraṃ khaṇaviddhaṃsī,",
  "anya-e1007n:181": "Kappantaṭṭhāyino guṇā.",
  "anya-e1007n:182": "24.",
  "anya-e1007n:183": "Kharānaṃ sīhabyagghānaṃ,",
  "anya-e1007n:184": "Saṅgamo no hisabbadā;",
  "anya-e1007n:185": "Tatheva byagghacittānaṃ,",
  "anya-e1007n:186": "Sajātikā khayonatā.",
  "anya-e1007n:187": "25.",
  "anya-e1007n:188": "Saka sādhupi no sādhū,",
  "anya-e1007n:189": "Yo ceñña duṭṭhakārako;",
  "anya-e1007n:190": "Bahūnaṃ sādhū pāyena,",
  "anya-e1007n:191": "Sa ve sādhūti vuccate.",
  "anya-e1007n:192": "26.",
  "anya-e1007n:193": "Bahūdake samuddepi,",
  "anya-e1007n:194": "Jalaṃ nattheva pātave;",
  "anya-e1007n:195": "Khuddake khatakūpamhi,",
  "anya-e1007n:196": "Sāduṃ atthi bahuṃ dakaṃ.",
  "anya-e1007n:197": "27.",
  "anya-e1007n:198": "Mā sīghaṃ vivareyyātha,",
  "anya-e1007n:199": "Nindituñca pasaṃsituṃ;",
  "anya-e1007n:200": "Mukhañhi vo kathādvāraṃ,",
  "anya-e1007n:201": "Nirundheyyātha sabbadā.",
  "anya-e1007n:202": "28.",
  "anya-e1007n:203": "Mā sīghaṃ vivareyyātha,",
  "anya-e1007n:204": "Cakkhuṃ vo dassituṃ piyaṃ;",
  "anya-e1007n:205": "Saṇikañhi piyalābhaṃ,",
  "anya-e1007n:206": "Dhanalābhaṃ turaṃ kare.",
  "anya-e1007n:207": "29.",
  "anya-e1007n:208": "Anārambho hi kammānaṃ,",
  "anya-e1007n:209": "Paṭhamaṃ buddhilakkhaṇaṃ;",
  "anya-e1007n:210": "Niṭṭhaṅgataṃ āraddhassa,",
  "anya-e1007n:211": "Dutiyaṃ buddhilakkhaṇaṃ.",
  "anya-e1007n:212": "Asamekkhitakammantaṃ,",
  "anya-e1007n:213": "Turitābhinipātinaṃ;",
  "anya-e1007n:214": "Sāni kammāni tappenti,",
  "anya-e1007n:215": "Uṇhaṃ vajjhohaṭaṃ mukhe.",
  "anya-e1007n:216": "30.",
  "anya-e1007n:217": "Aphalāni durantāni,",
  "anya-e1007n:218": "Janatā ninditāni ca;",
  "anya-e1007n:219": "Asakyāni ca kammāni,",
  "anya-e1007n:220": "Nārabhetha vicakkhaṇo.",
  "anya-e1007n:221": "31.",
  "anya-e1007n:222": "Ativirodhabhītānaṃ,",
  "anya-e1007n:223": "Saṅkitānaṃ pade pade;",
  "anya-e1007n:224": "Parappavādatāsānaṃ,",
  "anya-e1007n:225": "Dūrato yanti sampadā.",
  "anya-e1007n:226": "Saddamattaṃ na bhetabbaṃ,",
  "anya-e1007n:227": "Loko saddassa gocaro;",
  "anya-e1007n:228": "Yo ca saddaparittāso,",
  "anya-e1007n:229": "Vane bhantamigo hi so.",
  "anya-e1007n:230": "32.",
  "anya-e1007n:231": "Dvinnaṃ taṇḍulathūsānaṃ,",
  "anya-e1007n:232": "Viseso suṭṭhu khāyati;",
  "anya-e1007n:233": "Randhitopi siniddho no,",
  "anya-e1007n:234": "Thuso virasaphāruso;",
  "anya-e1007n:235": "Taṇḍulaṃ siniddhaṃ rasaṃ,",
  "anya-e1007n:236": "Evaṃ lokepi ñāyate.",
  "anya-e1007n:237": "33.",
  "anya-e1007n:238": "Eraṇḍaṃ nissitā valli,",
  "anya-e1007n:239": "Ruhate kiṃ yathābalaṃ;",
  "anya-e1007n:240": "Mahāsālaṃ sunissāya,",
  "anya-e1007n:241": "Ruhate brahataṃ gatā.",
  "anya-e1007n:242": "34.",
  "anya-e1007n:243": "Mettā hi sīmasambhedā,",
  "anya-e1007n:244": "Pakkhapāta vighātikā;",
  "anya-e1007n:245": "Pakkhapātena dukkhanti,",
  "anya-e1007n:246": "Nippakkho vasate sukhaṃ.",
  "anya-e1007n:247": "35.",
  "anya-e1007n:248": "Narā paññā ca laṅkārā,",
  "anya-e1007n:249": "Yathāṭhāne niyujjare;",
  "anya-e1007n:250": "No hi cūḷāmaṇi pāde,",
  "anya-e1007n:251": "Pādukā ca siropari.",
  "anya-e1007n:252": "Ukkuṭṭhe sūra micchanti;",
  "anya-e1007n:253": "Mantīsu akutūhalaṃ;",
  "anya-e1007n:254": "Viyañca annapānamhi;",
  "anya-e1007n:255": "Atthe jāte ca paṇḍitaṃ.",
  "anya-e1007n:256": "36.",
  "anya-e1007n:257": "Pamādo hi tamo loke,",
  "anya-e1007n:258": "Kālo coro bhayānako;",
  "anya-e1007n:259": "Kāyagehaṃ bahuchiddaṃ,",
  "anya-e1007n:260": "Kālacorassa coritaṃ.",
  "anya-e1007n:261": "37.",
  "anya-e1007n:262": "Cañcalo kāladāso hi,",
  "anya-e1007n:263": "Dhītimā kālaissaro;",
  "anya-e1007n:264": "Kālissaro raṭṭhissaraṃ,",
  "anya-e1007n:265": "Ativattati sabbaso.",
  "anya-e1007n:266": "38.",
  "anya-e1007n:267": "Viluppanti 8 dhanaṃ eke,",
  "anya-e1007n:268": "Kālameke anekkhakā;",
  "anya-e1007n:269": "Tesu kālavilopāva,",
  "anya-e1007n:270": "Bhayānakā tikakkhaḷā.",
  "anya-e1007n:271": "39.",
  "anya-e1007n:272": "Raññā raṭṭhahitaṃ kattā,",
  "anya-e1007n:273": "Rañño hitaṃ janehi ve;",
  "anya-e1007n:274": "Desso attahitaṃ dassī,",
  "anya-e1007n:275": "Gārayho kinnu kārako.",
  "anya-e1007n:276": "Attadatthaṃ paratthena;",
  "anya-e1007n:277": "Bahunāpi na hāpaye.",
  "anya-e1007n:278": "40.",
  "anya-e1007n:279": "Yassa upakāro dinno,",
  "anya-e1007n:280": "Upakāraṃ dade puna;",
  "anya-e1007n:281": "Tato pakāraṃ niccheyya,",
  "anya-e1007n:282": "Kataññū dullabho idha.",
  "anya-e1007n:283": "Saccaṃ kireva māhaṃsu,",
  "anya-e1007n:284": "Narā ekacciyā idha;",
  "anya-e1007n:285": "Kaṭṭhaṃ niplavitaṃ seyyo,",
  "anya-e1007n:286": "Natveve kacciyo naro.",
  "anya-e1007n:287": "41.",
  "anya-e1007n:288": "Kāruko sakapaññāya,",
  "anya-e1007n:289": "Mahagghaṃ dārukaṃ kare;",
  "anya-e1007n:290": "Tathā janopi attānaṃ,",
  "anya-e1007n:291": "Mahaggho lokamānito.",
  "anya-e1007n:292": "42.",
  "anya-e1007n:293": "Appaggho hi ayo hemaṃ,",
  "anya-e1007n:294": "Mahagghaṃ chindate yathā;",
  "anya-e1007n:295": "Nigguṇo saguṇaṃ loke,",
  "anya-e1007n:296": "Alakkhīca siriṃ tathā.",
  "anya-e1007n:297": "Sūrassatīnīti",
  "anya-e1007n:298": "Dutiyo bhāgo",
  "anya-e1007n:299": "1.",
  "anya-e1007n:300": "Dhanassa dubbidhaṃ kiccaṃ,",
  "anya-e1007n:301": "Pāpeti uṇṇataṃ dhaniṃ;",
  "anya-e1007n:302": "Adhaniṃ oṇataṃ loke,",
  "anya-e1007n:303": "Sañcine tena taṃ dhanaṃ.",
  "anya-e1007n:304": "2.",
  "anya-e1007n:305": "Vaṭṭate satataṃ sīghaṃ,",
  "anya-e1007n:306": "Kālacakkaṃ avāritaṃ;",
  "anya-e1007n:307": "Tena ghaṭī dinaṃ māso,",
  "anya-e1007n:308": "Vasso bhavatya cīrato.",
  "anya-e1007n:309": "3.",
  "anya-e1007n:310": "Sattunā na hi sandheyya,",
  "anya-e1007n:311": "Ekadā so bhayaṃ karo;",
  "anya-e1007n:312": "Sutattamapi pānīyaṃ,",
  "anya-e1007n:313": "Samayate nu pāvakaṃ.",
  "anya-e1007n:314": "4.",
  "anya-e1007n:315": "Vijahaṃ pakatiṃ yo hi,",
  "anya-e1007n:316": "Vikatiṃ puna gacchati;",
  "anya-e1007n:317": "Sabhāvena ākārena,",
  "anya-e1007n:318": "Vippallāsaṃ sa gacchati;",
  "anya-e1007n:319": "Saṃsumāra gatā godhā,",
  "anya-e1007n:320": "Yathā thī pumavesikā.",
  "anya-e1007n:321": "5.",
  "anya-e1007n:322": "Pakkhaṃ laddhāna uḍḍenti,",
  "anya-e1007n:323": "Upacikā hi vammikā;",
  "anya-e1007n:324": "Nikkhantā maraṇaṃ yanti,",
  "anya-e1007n:325": "Uppatā nippataṃ gatā.",
  "anya-e1007n:326": "6.",
  "anya-e1007n:327": "Sabbaṃpiyassa dajjeyya,",
  "anya-e1007n:328": "Nissesaṃ piyamānasaṃ;",
  "anya-e1007n:329": "Saddhācittaṃ tu no viññū,",
  "anya-e1007n:330": "Saddhāyiko pakkhalito.",
  "anya-e1007n:331": "7.",
  "anya-e1007n:332": "Sakkoti laṅghituṃ byāmaṃ,",
  "anya-e1007n:333": "Mahussāhena yo hi so;",
  "anya-e1007n:334": "Tadaḍḍhaṃ anussāhena,",
  "anya-e1007n:335": "Nossāho tesu thomito;",
  "anya-e1007n:336": "Mahussāho dukkho loke,",
  "anya-e1007n:337": "Anussāho sadā sukho.",
  "anya-e1007n:338": "8.",
  "anya-e1007n:339": "Dukkhamaṃ akkhamanto yo,",
  "anya-e1007n:340": "Piṭṭhikārīca dukkaraṃ,",
  "anya-e1007n:341": "Kadā labheyya so loke,",
  "anya-e1007n:342": "Sukhamaṃ sukaraṃ mudhā;",
  "anya-e1007n:343": "Paccakkhañhi so kareyya,",
  "anya-e1007n:344": "Dukkhamañcāpi dukkaraṃ.",
  "anya-e1007n:345": "9.",
  "anya-e1007n:346": "Anaggho manusso loke,",
  "anya-e1007n:347": "Tosanāposanādinā;",
  "anya-e1007n:348": "Tena so mahagghaṃ kammaṃ,",
  "anya-e1007n:349": "Kare lokahitāyutaṃ.",
  "anya-e1007n:350": "10.",
  "anya-e1007n:351": "Vātena nappabhijjanti,",
  "anya-e1007n:352": "Ninnā veḷū kasā naḷā;",
  "anya-e1007n:353": "Yathāvātaṃ nagacchanti,",
  "anya-e1007n:354": "Tathā care jane kadā.",
  "anya-e1007n:355": "11.",
  "anya-e1007n:356": "Purato ca pacchato ce,",
  "anya-e1007n:357": "Nissayo natthi passato;",
  "anya-e1007n:358": "Adhikaṃ vīriyaṃ hoti,",
  "anya-e1007n:359": "Attanātho tadā bhave.",
  "anya-e1007n:360": "12.",
  "anya-e1007n:361": "Khaṇaṃ ākhubilaṃ sīho,",
  "anya-e1007n:362": "Pāsāṇasakalākulaṃ;",
  "anya-e1007n:363": "Pappoti nakhabhaṅgaṃvā,",
  "anya-e1007n:364": "Phalaṃvā mūsiko bhave.",
  "anya-e1007n:365": "13.",
  "anya-e1007n:366": "Maggamuḷhā janā andhā,",
  "anya-e1007n:367": "Amaggā maggasaññino;",
  "anya-e1007n:368": "Tatheva duppaññā muḷhā,",
  "anya-e1007n:369": "Tathatthaṃ nāva bujjhare.",
  "anya-e1007n:370": "14.",
  "anya-e1007n:371": "Piyarūpaṃ vīrarūpaṃ,",
  "anya-e1007n:372": "Duvidhā rūpasampadā,",
  "anya-e1007n:373": "Nāriṃ icche piyarūpiṃ,",
  "anya-e1007n:374": "Purisaṃ vīrarūpakaṃ.",
  "anya-e1007n:375": "15.",
  "anya-e1007n:376": "Pajjalanti hi khajjotā,",
  "anya-e1007n:377": "Pakkhacālanakammunā;",
  "anya-e1007n:378": "Kusitā supitā nete,",
  "anya-e1007n:379": "Tathā janāpi kammikā.",
  "anya-e1007n:380": "16.",
  "anya-e1007n:381": "Kummasaṅkocamopamya,",
  "anya-e1007n:382": "Niggahamapi saṃkhame;",
  "anya-e1007n:383": "Pattakāle tu nītiñño,",
  "anya-e1007n:384": "Kaṇhasappova uṭṭhahe.",
  "anya-e1007n:385": "17.",
  "anya-e1007n:386": "Bhakkhasesaṃ nakhādanti,",
  "anya-e1007n:387": "Sīhā unnatacetasā;",
  "anya-e1007n:388": "Paraṃpi napaṇāmenti,",
  "anya-e1007n:389": "Vuddhikāmā tathā care.",
  "anya-e1007n:390": "18.",
  "anya-e1007n:391": "Na va vasso samuppanno,",
  "anya-e1007n:392": "Khīṇo purāṇahāyano;",
  "anya-e1007n:393": "Navavasse navā mettā,",
  "anya-e1007n:394": "Bhāvitabbā hitesinā.",
  "anya-e1007n:395": "19.",
  "anya-e1007n:396": "Santāpayanti kamayāpyabhujaṃ na rogā,",
  "anya-e1007n:397": "Dummantinaṃ kamupayanti na nītidosā;",
  "anya-e1007n:398": "Kaṃ srī na mānayati kaṃ na ca hanti maccu,",
  "anya-e1007n:399": "Kaṃ thīkatā na visayā paripīḷayanti.",
  "anya-e1007n:400": "(Vasantatilakāgāthā.)",
  "anya-e1007n:401": "20.",
  "anya-e1007n:402": "(Ka) byāmamattena daṇḍena,",
  "anya-e1007n:403": "Yolumbya udakaṃ mine;",
  "anya-e1007n:404": "Agambhīraṃ gambhīraṃvā,",
  "anya-e1007n:405": "Agādhe maññi gambhīraṃ.",
  "anya-e1007n:406": "(Kha)",
  "anya-e1007n:407": "Tathā mando saññāṇena,",
  "anya-e1007n:408": "Agādhe maññi paṇḍitaṃ;",
  "anya-e1007n:409": "Samāsamaṃ na jānāti,",
  "anya-e1007n:410": "Bahvappaṃ tikkhamandataṃ.",
  "anya-e1007n:411": "21.",
  "anya-e1007n:412": "Duṭṭhakamme saṅgamanti,",
  "anya-e1007n:413": "Chekakamme ca no idha;",
  "anya-e1007n:414": "Maccuṃ vahanti sīsena,",
  "anya-e1007n:415": "Te muḷhā muḷhasaṅgamā.",
  "anya-e1007n:416": "22.",
  "anya-e1007n:417": "Duvidho saṅgamo loke,",
  "anya-e1007n:418": "Ujuko kuṭilo bhave;",
  "anya-e1007n:419": "Ujukova pasaṃseyyo,",
  "anya-e1007n:420": "Nohyañño sājasaṅgamo.",
  "anya-e1007n:421": "Te iminā upāyena samaggā sammodamānā mahā bhittipiṭṭhikāya vasanti. (Mahosadhajātaka aṭṭhakathā)",
  "anya-e1007n:422": "23.",
  "anya-e1007n:423": "Yūthikā pupphate nohi,",
  "anya-e1007n:424": "Siñcitāpi punappunaṃ;",
  "anya-e1007n:425": "Pupphate sampatte kāle,",
  "anya-e1007n:426": "Evaṃ dhāretha vīriyaṃ.",
  "anya-e1007n:427": "24.",
  "anya-e1007n:428": "Dhanuccayo dhanakkhepo,",
  "anya-e1007n:429": "Duvidhā hi dhanākati;",
  "anya-e1007n:430": "Dhanuccaye nayo atthi,",
  "anya-e1007n:431": "Dhanakkhepamhi no idha.",
  "anya-e1007n:432": "25.",
  "anya-e1007n:433": "Amātā pitarasaṃ vaḍḍhaṃ,",
  "anya-e1007n:434": "Jūtakārañca cañcalaṃ;",
  "anya-e1007n:435": "Nālapeyya visesaññū,",
  "anya-e1007n:436": "Yadicche siddhi mattano.",
  "anya-e1007n:437": "Haliddirāgaṃ kapicittaṃ,",
  "anya-e1007n:438": "Purisaṃ rāgavirāginaṃ;",
  "anya-e1007n:439": "Edisaṃ tāta māsevi,",
  "anya-e1007n:440": "Nimmanussaṃpi ce siyā.",
  "anya-e1007n:441": "26.",
  "anya-e1007n:442": "Guṇā guṇaññūsu guṇā bhavanti,",
  "anya-e1007n:443": "Te nigguṇaṃ patvā bhavanti dosā;",
  "anya-e1007n:444": "Āsādyatoyā pabhavanti najjo,",
  "anya-e1007n:445": "Samudramāsajja bhavantyapeyyā.",
  "anya-e1007n:446": "(Upajātigāthā)",
  "anya-e1007n:447": "27.",
  "anya-e1007n:448": "Silārūpaṃ nimminanti,",
  "anya-e1007n:449": "Koṭṭetvāna punappunaṃ;",
  "anya-e1007n:450": "Koṭṭakova tathā bālā,",
  "anya-e1007n:451": "Sādhuṃ ovajja nimmitā.",
  "anya-e1007n:452": "Cāṇakyanītilā gāthā",
  "anya-e1007n:453": "Lālane bahavo dosā,",
  "anya-e1007n:454": "Tāḷane bahavo guṇā;",
  "anya-e1007n:455": "Tasmā puttañca sissañca,",
  "anya-e1007n:456": "Tāḷaye na ca lālaye.",
  "anya-e1007n:457": "28.",
  "anya-e1007n:458": "Atītassa hi mittassa,",
  "anya-e1007n:459": "Yo ce dosaṃ pakāsaye;",
  "anya-e1007n:460": "So have paccuppannassa,",
  "anya-e1007n:461": "Dosaṃ bhāseti ñāyati.",
  "anya-e1007n:462": "29.",
  "anya-e1007n:463": "Latāviya sevakā te,",
  "anya-e1007n:464": "Ye nissayaṃ palambare;",
  "anya-e1007n:465": "Nissayassa vināsena,",
  "anya-e1007n:466": "Bhūmyaṃ senti anāthakā.",
  "anya-e1007n:467": "30.",
  "anya-e1007n:468": "Dosasiṅgehi vijjhanto,",
  "anya-e1007n:469": "Mānakhūrehi akkamaṃ;",
  "anya-e1007n:470": "Bhayaṃ karoti lokamhi,",
  "anya-e1007n:471": "Gova bālo vihiṃsako.",
  "anya-e1007n:472": "31.",
  "anya-e1007n:473": "Ādo upari lokoyaṃ,",
  "anya-e1007n:474": "Ujulekhāya tiṭṭhati;",
  "anya-e1007n:475": "Musāvātehi taṃlokaṃ,",
  "anya-e1007n:476": "Nipātesi anajjavaṃ.",
  "anya-e1007n:477": "32.",
  "anya-e1007n:478": "Sughaṭaṃ kumbhakārena,",
  "anya-e1007n:479": "Nāraho paribhuñjituṃ;",
  "anya-e1007n:480": "Tathūpamāya vekkheyya,",
  "anya-e1007n:481": "Sakammaparakammani.",
  "anya-e1007n:482": "33.",
  "anya-e1007n:483": "Anantaraṃsī sūropi,",
  "anya-e1007n:484": "Nasakkoti ghanaṃ tamaṃ;",
  "anya-e1007n:485": "Vijjhituṃ raṃsiyā loke,",
  "anya-e1007n:486": "Tathā madanamohitā;",
  "anya-e1007n:487": "Nasakkonti madaṃ bhetvā,",
  "anya-e1007n:488": "Paññābhāya pabhāsituṃ.",
  "anya-e1007n:489": "34.",
  "anya-e1007n:490": "Khedaveraṃ daliddamhi,",
  "anya-e1007n:491": "Bhogimhi rogupaddavaṃ;",
  "anya-e1007n:492": "Dessaverañca āṇimhi,",
  "anya-e1007n:493": "Passe lokassa veritaṃ.",
  "anya-e1007n:494": "35.",
  "anya-e1007n:495": "Saṃladdhena subhogena,",
  "anya-e1007n:496": "Jīvaṃ suddhaṃ kare nijaṃ;",
  "anya-e1007n:497": "Seṭṭho so tena jīvena,",
  "anya-e1007n:498": "Jeguccho malajīviko.",
  "anya-e1007n:499": "36.",
  "anya-e1007n:500": "Vajira puppharāgānaṃ,",
  "anya-e1007n:501": "Visesaṃ yo nabujjhati;",
  "anya-e1007n:502": "Kathañhi so vikkīṇeyya,",
  "anya-e1007n:503": "Kīṇeyya vā yathātathaṃ.",
  "anya-e1007n:504": "37.",
  "anya-e1007n:505": "Kippīli kopi cintetvā,",
  "anya-e1007n:506": "Pabbataṃ bhettu mussahaṃ;",
  "anya-e1007n:507": "Abalā tanumajjhattā,",
  "anya-e1007n:508": "Cintā hasyāva sā mudhā.",
  "anya-e1007n:509": "38.",
  "anya-e1007n:510": "Jātamattaṃ na yo sattuṃ,",
  "anya-e1007n:511": "Rogañcūpasamaṃ naye;",
  "anya-e1007n:512": "Mahābalopi teneva,",
  "anya-e1007n:513": "Vuddhiṃpatvā sa haññate.",
  "anya-e1007n:514": "39.",
  "anya-e1007n:515": "Sajīvamaṃsabhakkhehi,",
  "anya-e1007n:516": "Sadāṭhīhi mukhehi bho;",
  "anya-e1007n:517": "Biḷārabyagghasīhānaṃ,",
  "anya-e1007n:518": "Nihīnāni anekadhā;",
  "anya-e1007n:519": "Tikkhāni kharavādāni,",
  "anya-e1007n:520": "Manussānaṃ mukhāni ve.",
  "anya-e1007n:521": "40.",
  "anya-e1007n:522": "Viluppantā vidhāvanti,",
  "anya-e1007n:523": "Sajīvavuttikammunā;",
  "anya-e1007n:524": "Janā tena vihaññanti,",
  "anya-e1007n:525": "Caranti dhammavemukhā.",
  "anya-e1007n:526": "41.",
  "anya-e1007n:527": "Sulabhaṃ lokiyaṃ loke,",
  "anya-e1007n:528": "Sāsanīyaṃva dullabhaṃ;",
  "anya-e1007n:529": "Dullabhaṃ taṃ vamaññanto,",
  "anya-e1007n:530": "Eso bālatamo bhave.",
  "anya-e1007n:531": "42.",
  "anya-e1007n:532": "Yo patittha agyāvāṭaṃ,",
  "anya-e1007n:533": "Mohā taṃ upakārituṃ;",
  "anya-e1007n:534": "Aññorohi tadā vāṭaṃ,",
  "anya-e1007n:535": "Dutīyo muḷhamuḷhako.",
  "anya-e1007n:536": "43.",
  "anya-e1007n:537": "Byaggho āvudhaviddho hi,",
  "anya-e1007n:538": "Akā duṭṭhāni ninnadaṃ;",
  "anya-e1007n:539": "Tatheva sādhusatthena,",
  "anya-e1007n:540": "Viddho bālo pakuppito.",
  "anya-e1007n:541": "44.",
  "anya-e1007n:542": "Pivanti lohitaṃ ḍaṃsā,",
  "anya-e1007n:543": "Anto tuṇḍena makkhikā;",
  "anya-e1007n:544": "Bahiddhā parivārenti,",
  "anya-e1007n:545": "Jano tena ḍaṃsāyaye.",
  "anya-e1007n:546": "45.",
  "anya-e1007n:547": "Adhanassa khaṇo appo,",
  "anya-e1007n:548": "Saddhammo appakālino;",
  "anya-e1007n:549": "Appako tena yuñjeyyuṃ,",
  "anya-e1007n:550": "Khaṇaṃ bahuṃ labhetave."
}