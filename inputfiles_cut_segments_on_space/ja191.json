{
  "ja191:0.1": "Jātaka ",
  "ja191:0.2": "Dukanipāta ",
  "ja191:0.3": "Ruhakavagga ",
  "ja191:0.4": "1. Ruhakajātaka ",
  "ja191:1.1": "“Api ruhaka chinnāpi, ",
  "ja191:1.2": "jiyā sandhīyate puna; ",
  "ja191:1.3": "Sandhīyassu purāṇiyā, ",
  "ja191:1.4": "mā kodhassa vasaṁ gami”. ",
  "ja191:2.1": "“Vijjamānesu vākesu, ",
  "ja191:2.2": "vijjamānesu kārisu; ",
  "ja191:2.3": "Aññaṁ jiyaṁ karissāmi, ",
  "ja191:2.4": "alaññeva purāṇiyā”ti. ",
  "ja191:3.1": "Ruhakajātakaṁ paṭhamaṁ. "
}