{
  "anya-e1009n6:0": "Paññā-niddesa",
  "anya-e1009n6:1": "55.",
  "anya-e1009n6:2": " Paññaṁ pathamamesehi,",
  "anya-e1009n6:3": "Paññā-balaṁ bahuttamaṁ;",
  "anya-e1009n6:4": "Kula-putta balaṁ paññā,",
  "anya-e1009n6:5": "Kiṁhināma na sādhyati.",
  "anya-e1009n6:6": "56.",
  "anya-e1009n6:7": " Aneka-saṁsayucchedi ,",
  "anya-e1009n6:8": "Parokkhatthassa dassakaṁ;",
  "anya-e1009n6:9": "Sabbassa locanaṁ satthaṁ,",
  "anya-e1009n6:10": "Yassa natthyandhameva so.",
  "anya-e1009n6:11": "57.",
  "anya-e1009n6:12": " Paññā sutaṁ vinicchindi,",
  "anya-e1009n6:13": "Kitti-siloka-vaḍḍhanī;",
  "anya-e1009n6:14": "Paññāsahito naro idha,",
  "anya-e1009n6:15": "Api sukhānivindati.",
  "anya-e1009n6:16": "58.",
  "anya-e1009n6:17": " Sabbaññubuddha-pacceka ,",
  "anya-e1009n6:18": "Catusacca-sutā iti;",
  "anya-e1009n6:19": "Catu-buddhesu ekova,",
  "anya-e1009n6:20": "Bahussuto naro bhave.",
  "anya-e1009n6:21": "59.",
  "anya-e1009n6:22": " Lekhacheko vācacheko,",
  "anya-e1009n6:23": "Ganthacheko suvācako;",
  "anya-e1009n6:24": "Vidhāyakacheko sūro,",
  "anya-e1009n6:25": "Niddukkhova sakammani.",
  "anya-e1009n6:26": "60.",
  "anya-e1009n6:27": " Paññā hi seṭṭhā kusalā vadanti,",
  "anya-e1009n6:28": "Nakkhatta-rājāriva tārakānaṁ;",
  "anya-e1009n6:29": "Sīlaṁ sirīcāpi satañca dhammo,",
  "anya-e1009n6:30": "Anvāyikā paññāvato bhavanti.",
  "anya-e1009n6:31": "61.",
  "anya-e1009n6:32": " Sevetha buddhe nipuṇe bahussutte,",
  "anya-e1009n6:33": "Uggāhako ca paripucchako;",
  "anya-e1009n6:34": "Suṇeyya sakkaccaṁ subhāsitāni,",
  "anya-e1009n6:35": "Evaṁ karo paññavā hoti macco.",
  "anya-e1009n6:36": "62.",
  "anya-e1009n6:37": " Vayena yasa-pucchāhi,",
  "anya-e1009n6:38": "Tiṭṭha-vāsena yoniso;",
  "anya-e1009n6:39": "Sākacchā snehasaṁsevā,",
  "anya-e1009n6:40": "Patirūpa-vāsenaca.",
  "anya-e1009n6:41": "63.",
  "anya-e1009n6:42": " Etāni aṭṭhaṭhānāni,",
  "anya-e1009n6:43": "Buddhi-visada-kāraṇā;",
  "anya-e1009n6:44": "Yesaṁ etāni sambhonti,",
  "anya-e1009n6:45": "Tesaṁ buddhi pabhijjati.",
  "anya-e1009n6:46_0": "64. Cattārome bhikkhave dhammā paññāvuddhiyā saṁvattanti. Katame cattāro. Sappurisasaṁsevo",
  "anya-e1009n6:46_1": "saddhammasavanaṁ yoniso manasikāro dhammānudhamma-paṭipatti. Ime kho bhikkhave cattāro",
  "anya-e1009n6:46_2": "dhammā paññā-vuddhiyā saṁvattantītti.",
  "anya-e1009n6:47": "65.",
  "anya-e1009n6:48": " Cakkhupasāda-sampanno ,",
  "anya-e1009n6:49": "Acchimantañca passati;",
  "anya-e1009n6:50": "Andhaṁ kāṇaṁ su-passantaṁ,",
  "anya-e1009n6:51": "Andho sabbaṁ na passati.",
  "anya-e1009n6:52": "66.",
  "anya-e1009n6:53": " Passatti passo passantaṁ,",
  "anya-e1009n6:54": "A-passantañca passati;",
  "anya-e1009n6:55": "A-passanto a-passantaṁ,",
  "anya-e1009n6:56": "Passantañca na passati.",
  "anya-e1009n6:57": "67.",
  "anya-e1009n6:58": " Pākaṭaṁ a-paṭicchannaṁ,",
  "anya-e1009n6:59": "Rūpaṁ pasāda-cakkhunā;",
  "anya-e1009n6:60": "Nāññaṁ passatti sabbaṁpi,",
  "anya-e1009n6:61": "Tathato ñāṇa-cakkhunā.",
  "anya-e1009n6:62": "68.",
  "anya-e1009n6:63": " Sujanāsujanā sabbe,",
  "anya-e1009n6:64": "Guṇenāpi vivekino;",
  "anya-e1009n6:65": "Vivekaṁ na samāyanti,",
  "anya-e1009n6:66": "A-vivekījanantike.",
  "anya-e1009n6:67": "69.",
  "anya-e1009n6:68": " Yo ca uppatitaṁ atthaṁ,",
  "anya-e1009n6:69": "Na khippamanubujjhati;",
  "anya-e1009n6:70": "A-mittavasamanveti,",
  "anya-e1009n6:71": "Pacchā ca anutappati.",
  "anya-e1009n6:72": "70.",
  "anya-e1009n6:73": " Evaṁ mahiddhikā paññā,",
  "anya-e1009n6:74": "Nipuṇā sādhucintinī;",
  "anya-e1009n6:75": "Diṭṭhadhamma-hitatthāya,",
  "anya-e1009n6:76": "Samparāya-sukhāya vā.",
  "anya-e1009n6:77": "71.",
  "anya-e1009n6:78": " Taṁ balānaṁ balaṁ seṭṭha,",
  "anya-e1009n6:79": "Agga paññābalaṁ balaṁ;",
  "anya-e1009n6:80": "Paññābalenupatthaddho,",
  "anya-e1009n6:81": "Etthaṁ vindati paṇḍitto.",
  "anya-e1009n6:82": "72.",
  "anya-e1009n6:83": " Yena ñāṇena bujjhanti,",
  "anya-e1009n6:84": "Ariyā kata-kiccataṁ;",
  "anya-e1009n6:85": "Taṁ ñāṇa-ratanaṁ laddhaṁ,",
  "anya-e1009n6:86": "Vāyāmetha jinorasā.",
  "anya-e1009n6:87": "73.",
  "anya-e1009n6:88": " Paññāratanamālassa ,",
  "anya-e1009n6:89": "Na ciraṁ vattate bhavo;",
  "anya-e1009n6:90": "Khippaṁ dasseti amataṁ,",
  "anya-e1009n6:91": "Na ca so rocate bhave.",
  "anya-e1009n6:92": "74.",
  "anya-e1009n6:93": " Pamādamanuyuñjanti ,",
  "anya-e1009n6:94": "Bālā dummedhino janā;",
  "anya-e1009n6:95": "Appamādañca medhāvī,",
  "anya-e1009n6:96": "Dhanaṁ seṭṭhaṁva rakkhati.",
  "anya-e1009n6:97": "75.",
  "anya-e1009n6:98": " Dhana-puñña-dhī-lābhena ,",
  "anya-e1009n6:99": "Kālaṁ khiyyati paṇḍito;",
  "anya-e1009n6:100": "Kīḷanena ca dummedho,",
  "anya-e1009n6:101": "Niddāya kalahena vā.",
  "anya-e1009n6:102": "76.",
  "anya-e1009n6:103": " Pamādaṁ appamādena,",
  "anya-e1009n6:104": "Yadā nūdati paṇḍito;",
  "anya-e1009n6:105": "Paññāpāsāda-māruyha,",
  "anya-e1009n6:106": "A-soko sokiniṁ pajaṁ,",
  "anya-e1009n6:107": "Pabbataṭṭhova bhūmaṭṭhe,",
  "anya-e1009n6:108": "Dhīro bāle avekkhati.",
  "anya-e1009n6:109": "77.",
  "anya-e1009n6:110": " Natti attasamaṁ pemaṁ,",
  "anya-e1009n6:111": "Natthi dhaññasamaṁ dhanaṁ;",
  "anya-e1009n6:112": "Natthi paññāsamā ābhā,",
  "anya-e1009n6:113": "Vuṭṭhi ve paramā sarā.",
  "anya-e1009n6:114": "78.",
  "anya-e1009n6:115": " Diṭṭhe dhamme ca yo attho,",
  "anya-e1009n6:116": "Yo cattho samparāyiko;",
  "anya-e1009n6:117": "Atthābhisamayā dhīro,",
  "anya-e1009n6:118": "Paṇḍitoti pavuccati.",
  "anya-e1009n6:119": "79.",
  "anya-e1009n6:120": " Na tena paṇḍito hoti,",
  "anya-e1009n6:121": "Yāvatā bahu bhāsati;",
  "anya-e1009n6:122": "Khemī a-verī a-bhayo,",
  "anya-e1009n6:123": "Paṇḍitotti pavuccati.",
  "anya-e1009n6:124": "80.",
  "anya-e1009n6:125": " Yamhi saccañca dhammo ca,",
  "anya-e1009n6:126": "A-hiṁsā saṁyamo damo;",
  "anya-e1009n6:127": "Sa ve vantamalo dhīro,",
  "anya-e1009n6:128": "Thero iti pavuccatti.",
  "anya-e1009n6:129": "81.",
  "anya-e1009n6:130": " Saka-guṇaṁ saka-dosaṁ,",
  "anya-e1009n6:131": "Yo jānāti sapaṇḍito;",
  "anya-e1009n6:132": "Para-guṇaṁ para-dosaṁ,",
  "anya-e1009n6:133": "Yo jānāti sapaṇḍito.",
  "anya-e1009n6:134": "82.",
  "anya-e1009n6:135": " Sati-vīriya-paññāya ,",
  "anya-e1009n6:136": "Yo karoti iriyāpathe;",
  "anya-e1009n6:137": "So paṇḍito have bhave,",
  "anya-e1009n6:138": "Ubhayattha-pariggaho.",
  "anya-e1009n6:139": "83.",
  "anya-e1009n6:140": " Kataññū vijjā-sampanno,",
  "anya-e1009n6:141": "Jātimā dhanavā have;",
  "anya-e1009n6:142": "So vicāraṇa-sīlo ca,",
  "anya-e1009n6:143": "Niddukkho paṇḍito bhave.",
  "anya-e1009n6:144": "Sabbe kammassakā sattā,",
  "anya-e1009n6:145": "Kammaṁ satte vibhajjati.",
  "anya-e1009n6:146": "84.",
  "anya-e1009n6:147": " Yo passati paccakkhatthaṁ,",
  "anya-e1009n6:148": "Yo ca saṁsāratthaṁ tesu;",
  "anya-e1009n6:149": "Pacchimova pūjanīyo,",
  "anya-e1009n6:150": "Ubhayattha-sudiṭṭhattā.",
  "anya-e1009n6:151": "85.",
  "anya-e1009n6:152": " Appena anavajjena,",
  "anya-e1009n6:153": "Santuṭṭho sulabhena ca;",
  "anya-e1009n6:154": "Mattaññū subharo hutvā,",
  "anya-e1009n6:155": "Careyya paṇḍito naro.",
  "anya-e1009n6:156": "86.",
  "anya-e1009n6:157": " Attānameva pathamaṁ,",
  "anya-e1009n6:158": "Patirūpe nivesaye;",
  "anya-e1009n6:159": "Athaññamanusāseyya,",
  "anya-e1009n6:160": "Na kilisseyya paṇḍito.",
  "anya-e1009n6:161": "87.",
  "anya-e1009n6:162": " Uttama-parisāya ve,",
  "anya-e1009n6:163": "Utttamaṁ vācamuttamo;",
  "anya-e1009n6:164": "Bhaṇeyyākhepa-vitthāraṁ,",
  "anya-e1009n6:165": "Sā anagghyā lokattaye.",
  "anya-e1009n6:166": "88.",
  "anya-e1009n6:167": " Paṇḍitassa subhāsittaṁ,",
  "anya-e1009n6:168": "Paṇḍitova sujāniyā;",
  "anya-e1009n6:169": "Dummedho taṁ na jānāti,",
  "anya-e1009n6:170": "Dhīro dhīraṁ mamāyati.",
  "anya-e1009n6:171": "89.",
  "anya-e1009n6:172": " Yena kenaci vaṇṇena,",
  "anya-e1009n6:173": "Paro labhati rūppanaṁ;",
  "anya-e1009n6:174": "Attho vācāya ce hoti,",
  "anya-e1009n6:175": "Taṁ na bhāseyya paṇḍito.",
  "anya-e1009n6:176": "90.",
  "anya-e1009n6:177": " Bhuñjanatthaṁ kathanatthaṁ,",
  "anya-e1009n6:178": "Mukhaṁ hotītti no vade;",
  "anya-e1009n6:179": "Yaṁvā taṁvā mukhāruḷhaṁ,",
  "anya-e1009n6:180": "Vacanaṁ paṇḍito naro.",
  "anya-e1009n6:181": "91.",
  "anya-e1009n6:182": " Para-sattitto sa-sattiṁ,",
  "anya-e1009n6:183": "Dujjāno hi naro mite;",
  "anya-e1009n6:184": "Ce jāne saka-sattiṁca,",
  "anya-e1009n6:185": "Kā kathā para-sattiyā.",
  "anya-e1009n6:186": "92.",
  "anya-e1009n6:187": " Katta-guṇaṁ paresaṁ yo,",
  "anya-e1009n6:188": "Paṭikaroti paṇḍito;",
  "anya-e1009n6:189": "Jānāti so ācikkhati,",
  "anya-e1009n6:190": "Na bālo guṇa-māmako.",
  "anya-e1009n6:191": "93.",
  "anya-e1009n6:192": " Pabhūtaṁ neva kātabbaṁ,",
  "anya-e1009n6:193": "Bhavissaṁ neva cintaye;",
  "anya-e1009n6:194": "Vattamānena kālena,",
  "anya-e1009n6:195": "Vicaranti vicakkhaṇā.",
  "anya-e1009n6:196": "94.",
  "anya-e1009n6:197": " Dhammesu sati icchitā,",
  "anya-e1009n6:198": "Rasesu loṇamicchitaṁ;",
  "anya-e1009n6:199": "Rāja-kiccesu amaccaṁ,",
  "anya-e1009n6:200": "Sabba-ṭhānesu paṇḍitaṁ.",
  "anya-e1009n6:201": "95.",
  "anya-e1009n6:202": " Khattiyo seṭṭho jane tasmiṁ,",
  "anya-e1009n6:203": "Ye gottapaṭisārino;",
  "anya-e1009n6:204": "Vijjā-caraṇa-sampanno,",
  "anya-e1009n6:205": "So seṭṭho deva-mānuse.",
  "anya-e1009n6:206": "96.",
  "anya-e1009n6:207": " Sambuddho dvipadaṁ seṭṭho,",
  "anya-e1009n6:208": "Ājānīyo catuppadaṁ;",
  "anya-e1009n6:209": "Sussusā seṭṭhā bhariyānaṁ,",
  "anya-e1009n6:210": "Yo ca puttānamassavo.",
  "anya-e1009n6:211": "97.",
  "anya-e1009n6:212": " Sātthako ca a-sammoho,",
  "anya-e1009n6:213": "Sappāyo gocaro tathā;",
  "anya-e1009n6:214": "Cattārimāni sikkheyyuṁ,",
  "anya-e1009n6:215": "Sampajaññābhivaḍḍhakā.",
  "anya-e1009n6:216": "98.",
  "anya-e1009n6:217": " Paññañca kho a-sussusaṁ,",
  "anya-e1009n6:218": "Na koci adhigacchati;",
  "anya-e1009n6:219": "Bahussutaṁ anāgamma,",
  "anya-e1009n6:220": "Dhammaṭṭhaṁ a-vinibbhajaṁ.",
  "anya-e1009n6:221": "99.",
  "anya-e1009n6:222": " Adhippāyo sudubbodho,",
  "anya-e1009n6:223": "Yasmā vijjati pāḷiyaṁ;",
  "anya-e1009n6:224": "Tasmā upaṭṭhahaṁ gaṇhe,",
  "anya-e1009n6:225": "Garuṁ garumataṁ vidū."
}