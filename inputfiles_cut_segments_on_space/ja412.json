{
  "ja412:0.1": "Jātaka ",
  "ja412:0.2": "Sattakanipāta ",
  "ja412:0.3": "Gandhāravagga ",
  "ja412:0.4": "7. Koṭasimbalijātaka ",
  "ja412:1.1": "“Ahaṁ dasasataṁbyāmaṁ, ",
  "ja412:1.2": "uragamādāya āgato; ",
  "ja412:1.3": "Tañca mañca mahākāyaṁ, ",
  "ja412:1.4": "dhārayaṁ nappavedhasi. ",
  "ja412:2.1": "Athimaṁ khuddakaṁ pakkhiṁ, ",
  "ja412:2.2": "Appamaṁsataraṁ mayā; ",
  "ja412:2.3": "Dhārayaṁ byathasi bhītā, ",
  "ja412:2.4": "Kamatthaṁ koṭasimbali”. ",
  "ja412:3.1": "“Maṁsabhakkho tuvaṁ rāja, ",
  "ja412:3.2": "phalabhakkho ayaṁ dijo; ",
  "ja412:3.3": "Ayaṁ nigrodhabījāni, ",
  "ja412:3.4": "pilakkhudumbarāni ca; ",
  "ja412:3.5": "Assatthāni ca bhakkhitvā, ",
  "ja412:3.6": "khandhe me ohadissati. ",
  "ja412:4.1": "Te rukkhā saṁvirūhanti, ",
  "ja412:4.2": "mama passe nivātajā; ",
  "ja412:4.3": "Te maṁ pariyonandhissanti, ",
  "ja412:4.4": "arukkhaṁ maṁ karissare. ",
  "ja412:5.1": "Santi aññepi rukkhā se, ",
  "ja412:5.2": "mūlino khandhino dumā; ",
  "ja412:5.3": "Iminā sakuṇajātena, ",
  "ja412:5.4": "bījamāharitā hatā. ",
  "ja412:6.1": "Ajjhārūhābhivaḍḍhanti, ",
  "ja412:6.2": "brahantampi vanappatiṁ; ",
  "ja412:6.3": "Tasmā rāja pavedhāmi, ",
  "ja412:6.4": "sampassaṁnāgataṁ bhayaṁ”. ",
  "ja412:7.1": "“Saṅkeyya saṅkitabbāni, ",
  "ja412:7.2": "rakkheyyānāgataṁ bhayaṁ; ",
  "ja412:7.3": "Anāgatabhayā dhīro, ",
  "ja412:7.4": "ubho loke avekkhatī”ti. ",
  "ja412:8.1": "Koṭasimbalijātakaṁ sattamaṁ. "
}