{
  "ja208:0.1": "Jātaka ",
  "ja208:0.2": "Dukanipāta ",
  "ja208:0.3": "Nataṁdaḷhavagga ",
  "ja208:0.4": "8. Susumārajātaka ",
  "ja208:1.1": "“Alaṁ metehi ambehi, ",
  "ja208:1.2": "jambūhi panasehi ca; ",
  "ja208:1.3": "Yāni pāraṁ samuddassa, ",
  "ja208:1.4": "varaṁ mayhaṁ udumbaro. ",
  "ja208:2.1": "Mahatī vata te bondi, ",
  "ja208:2.2": "na ca paññā tadūpikā; ",
  "ja208:2.3": "Susumāra vañcito mesi, ",
  "ja208:2.4": "gaccha dāni yathāsukhan”ti. ",
  "ja208:3.1": "Susumārajātakaṁ aṭṭhamaṁ. "
}