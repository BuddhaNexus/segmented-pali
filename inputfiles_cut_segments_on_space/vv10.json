{
  "vv10:0.1": "Vimānavatthu ",
  "vv10:0.2": "Itthivimāna ",
  "vv10:0.3": "Pīṭhavagga ",
  "vv10:0.4": "10. Tiladakkhiṇavimānavatthu ",
  "vv10:1.1": "“Abhikkantena vaṇṇena, ",
  "vv10:1.2": "yā tvaṁ tiṭṭhasi devate; ",
  "vv10:1.3": "Obhāsentī disā sabbā, ",
  "vv10:1.4": "osadhī viya tārakā. ",
  "vv10:2.1": "Kena tetādiso vaṇṇo, ",
  "vv10:2.2": "kena te idha mijjhati; ",
  "vv10:2.3": "Uppajjanti ca te bhogā, ",
  "vv10:2.4": "ye keci manaso piyā. ",
  "vv10:3.1": "Pucchāmi taṁ devi mahānubhāve, ",
  "vv10:3.2": "Manussabhūtā kimakāsi puññaṁ; ",
  "vv10:3.3": "Kenāsi evaṁ jalitānubhāvā, ",
  "vv10:3.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti. ",
  "vv10:4.1": "Sā devatā attamanā, ",
  "vv10:4.2": "moggallānena pucchitā; ",
  "vv10:4.3": "Pañhaṁ puṭṭhā viyākāsi, ",
  "vv10:4.4": "yassa kammassidaṁ phalaṁ. ",
  "vv10:5.1": "“Ahaṁ manussesu manussabhūtā, ",
  "vv10:5.2": "Purimāya jātiyā manussaloke. ",
  "vv10:6.1": "Addasaṁ virajaṁ buddhaṁ, ",
  "vv10:6.2": "vippasannamanāvilaṁ; ",
  "vv10:6.3": "Āsajja dānaṁ adāsiṁ, ",
  "vv10:6.4": "akāmā tiladakkhiṇaṁ; ",
  "vv10:6.5": "Dakkhiṇeyyassa buddhassa, ",
  "vv10:6.6": "pasannā sehi pāṇibhi. ",
  "vv10:7.1": "Tena metādiso vaṇṇo, ",
  "vv10:7.2": "tena me idha mijjhati; ",
  "vv10:7.3": "Uppajjanti ca me bhogā, ",
  "vv10:7.4": "ye keci manaso piyā. ",
  "vv10:8.1": "Akkhāmi te bhikkhu mahānubhāva, ",
  "vv10:8.2": "Manussabhūtā yamakāsi puññaṁ; ",
  "vv10:8.3": "Tenamhi evaṁ jalitānubhāvā, ",
  "vv10:8.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti. ",
  "vv10:9.1": "Tiladakkhiṇavimānaṁ dasamaṁ. "
}