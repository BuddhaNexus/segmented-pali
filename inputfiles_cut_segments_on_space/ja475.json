{
  "ja475:0.1": "Jātaka ",
  "ja475:0.2": "Terasakanipāta ",
  "ja475:0.3": "Ambavagga ",
  "ja475:0.4": "2. Phandanajātaka ",
  "ja475:1.1": "“Kuṭhārihattho puriso, ",
  "ja475:1.2": "vanamogayha tiṭṭhasi; ",
  "ja475:1.3": "Puṭṭho me samma akkhāhi, ",
  "ja475:1.4": "kiṁ dāruṁ chetumicchasi”. ",
  "ja475:2.1": "“Isso vanāni carasi, ",
  "ja475:2.2": "samāni visamāni ca; ",
  "ja475:2.3": "Puṭṭho me samma akkhāhi, ",
  "ja475:2.4": "kiṁ dāruṁ nemiyā daḷhaṁ”. ",
  "ja475:3.1": "“Neva sālo na khadiro, ",
  "ja475:3.2": "nāssakaṇṇo kuto dhavo; ",
  "ja475:3.3": "Rukkho ca phandano nāma, ",
  "ja475:3.4": "taṁ dāruṁ nemiyā daḷhaṁ”. ",
  "ja475:4.1": "“Kīdisānissa pattāni, ",
  "ja475:4.2": "khandho vā pana kīdiso; ",
  "ja475:4.3": "Puṭṭho me samma akkhāhi, ",
  "ja475:4.4": "yathā jānemu phandanaṁ”. ",
  "ja475:5.1": "“Yassa sākhā palambanti, ",
  "ja475:5.2": "namanti na ca bhañjare; ",
  "ja475:5.3": "So rukkho phandano nāma, ",
  "ja475:5.4": "yassa mūle ahaṁ ṭhito. ",
  "ja475:6.1": "Arānaṁ cakkanābhīnaṁ, ",
  "ja475:6.2": "īsānemirathassa ca; ",
  "ja475:6.3": "Sabbassa te kammaniyo, ",
  "ja475:6.4": "ayaṁ hessati phandano”. ",
  "ja475:7.1": "“Iti phandanarukkhopi, ",
  "ja475:7.2": "tāvade ajjhabhāsatha; ",
  "ja475:7.3": "Mayhampi vacanaṁ atthi, ",
  "ja475:7.4": "bhāradvāja suṇohi me. ",
  "ja475:8.1": "Issassa upakkhandhamhā, ",
  "ja475:8.2": "Ukkacca caturaṅgulaṁ; ",
  "ja475:8.3": "Tena nemiṁ pasāresi, ",
  "ja475:8.4": "Evaṁ daḷhataraṁ siyā. ",
  "ja475:9.1": "Iti phandanarukkhopi, ",
  "ja475:9.2": "veraṁ appesi tāvade; ",
  "ja475:9.3": "Jātānañca ajātānaṁ, ",
  "ja475:9.4": "issānaṁ dukkhamāvahi. ",
  "ja475:10.1": "Iccevaṁ phandano issaṁ, ",
  "ja475:10.2": "isso ca pana phandanaṁ; ",
  "ja475:10.3": "Aññamaññaṁ vivādena, ",
  "ja475:10.4": "aññamaññamaghātayuṁ. ",
  "ja475:11.1": "Evameva manussānaṁ, ",
  "ja475:11.2": "vivādo yattha jāyati; ",
  "ja475:11.3": "Mayūranaccaṁ naccanti, ",
  "ja475:11.4": "yathā te issaphandanā. ",
  "ja475:12.1": "Taṁ vo vadāmi bhaddaṁ vo, ",
  "ja475:12.2": "yāvantettha samāgatā; ",
  "ja475:12.3": "Sammodatha mā vivadatha, ",
  "ja475:12.4": "mā hotha issaphandanā. ",
  "ja475:13.1": "Sāmaggimeva sikkhetha, ",
  "ja475:13.2": "buddhehetaṁ pasaṁsitaṁ; ",
  "ja475:13.3": "Sāmaggirato dhammaṭṭho, ",
  "ja475:13.4": "yogakkhemā na dhaṁsatī”ti. ",
  "ja475:14.1": "Phandanajātakaṁ dutiyaṁ. "
}