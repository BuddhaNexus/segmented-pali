{
  "ja379:0.1": "Jātaka ",
  "ja379:0.2": "Chakkanipāta ",
  "ja379:0.3": "Avāriyavagga ",
  "ja379:0.4": "4. Nerujātaka ",
  "ja379:1.1": "“Kākolā kākasaṅghā ca, ",
  "ja379:1.2": "mayañca patataṁ varā; ",
  "ja379:1.3": "Sabbeva sadisā homa, ",
  "ja379:1.4": "imaṁ āgamma pabbataṁ. ",
  "ja379:2.1": "Idha sīhā ca byagghā ca, ",
  "ja379:2.2": "siṅgālā ca migādhamā; ",
  "ja379:2.3": "Sabbeva sadisā honti, ",
  "ja379:2.4": "ayaṁ ko nāma pabbato”. ",
  "ja379:3.1": "“Imaṁ nerūti jānanti, ",
  "ja379:3.2": "manussā pabbatuttamaṁ; ",
  "ja379:3.3": "Idha vaṇṇena sampannā, ",
  "ja379:3.4": "vasanti sabbapāṇino”. ",
  "ja379:4.1": "“Amānanā yattha siyā, ",
  "ja379:4.2": "santānaṁ vā vimānanā; ",
  "ja379:4.3": "Hīnasammānanā vāpi, ",
  "ja379:4.4": "na tattha vasatiṁ vase. ",
  "ja379:5.1": "Yatthālaso ca dakkho ca, ",
  "ja379:5.2": "sūro bhīru ca pūjiyā; ",
  "ja379:5.3": "Na tattha santo vasanti, ",
  "ja379:5.4": "avisesakare nare. ",
  "ja379:6.1": "Nāyaṁ neru vibhajati, ",
  "ja379:6.2": "hīnaukkaṭṭhamajjhime; ",
  "ja379:6.3": "Avisesakaro neru, ",
  "ja379:6.4": "handa neruṁ jahāmase”ti. ",
  "ja379:7.1": "Nerujātakaṁ catutthaṁ. "
}