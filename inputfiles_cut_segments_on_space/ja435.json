{
  "ja435:0.1": "Jātaka ",
  "ja435:0.2": "Navakanipāta ",
  "ja435:0.3": "Gijjhavagga ",
  "ja435:0.4": "9. Haliddirāgajātaka ",
  "ja435:1.1": "“Sutitikkhaṁ araññamhi, ",
  "ja435:1.2": "pantamhi sayanāsane; ",
  "ja435:1.3": "Ye ca gāme titikkhanti, ",
  "ja435:1.4": "te uḷāratarā tayā”. ",
  "ja435:2.1": "“Araññā gāmamāgamma, ",
  "ja435:2.2": "kiṁsīlaṁ kiṁvataṁ ahaṁ; ",
  "ja435:2.3": "Purisaṁ tāta seveyyaṁ, ",
  "ja435:2.4": "taṁ me akkhāhi pucchito”. ",
  "ja435:3.1": "“Yo te vissāsaye tāta, ",
  "ja435:3.2": "vissāsañca khameyya te; ",
  "ja435:3.3": "Sussūsī ca titikkhī ca, ",
  "ja435:3.4": "taṁ bhajehi ito gato. ",
  "ja435:4.1": "Yassa kāyena vācāya, ",
  "ja435:4.2": "manasā natthi dukkaṭaṁ; ",
  "ja435:4.3": "Urasīva patiṭṭhāya, ",
  "ja435:4.4": "taṁ bhajehi ito gato. ",
  "ja435:5.1": "Yo ca dhammena carati, ",
  "ja435:5.2": "carantopi na maññati; ",
  "ja435:5.3": "Visuddhakāriṁ sappaññaṁ, ",
  "ja435:5.4": "taṁ bhajehi ito gato. ",
  "ja435:6.1": "Haliddirāgaṁ kapicittaṁ, ",
  "ja435:6.2": "purisaṁ rāgavirāginaṁ; ",
  "ja435:6.3": "Tādisaṁ tāta mā sevi, ",
  "ja435:6.4": "nimmanussampi ce siyā. ",
  "ja435:7.1": "Āsīvisaṁva kupitaṁ, ",
  "ja435:7.2": "mīḷhalittaṁ mahāpathaṁ; ",
  "ja435:7.3": "Ārakā parivajjehi, ",
  "ja435:7.4": "yānīva visamaṁ pathaṁ. ",
  "ja435:8.1": "Anatthā tāta vaḍḍhanti, ",
  "ja435:8.2": "bālaṁ accupasevato; ",
  "ja435:8.3": "Māssu bālena saṅgacchi, ",
  "ja435:8.4": "amitteneva sabbadā. ",
  "ja435:9.1": "Taṁ tāhaṁ tāta yācāmi, ",
  "ja435:9.2": "karassu vacanaṁ mama; ",
  "ja435:9.3": "Māssu bālena saṅgacchi, ",
  "ja435:9.4": "dukkho bālehi saṅgamo”ti. ",
  "ja435:10.1": "Haliddirāgajātakaṁ navamaṁ. "
}