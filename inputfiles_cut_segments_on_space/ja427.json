{
  "ja427:0.1": "Jātaka ",
  "ja427:0.2": "Navakanipāta ",
  "ja427:0.3": "Gijjhavagga ",
  "ja427:0.4": "1. Gijjhajātaka ",
  "ja427:1.1": "“Parisaṅkupatho nāma, ",
  "ja427:1.2": "gijjhapantho sanantano; ",
  "ja427:1.3": "Tatrāsi mātāpitaro, ",
  "ja427:1.4": "gijjho posesi jiṇṇake; ",
  "ja427:1.5": "Tesaṁ ajagaramedaṁ, ",
  "ja427:1.6": "accahāsi bahuttaso. ",
  "ja427:2.1": "Pitā ca puttaṁ avaca, ",
  "ja427:2.2": "Jānaṁ uccaṁ papātinaṁ; ",
  "ja427:2.3": "Supattaṁ thāmasampannaṁ, ",
  "ja427:2.4": "Tejassiṁ dūragāminaṁ. ",
  "ja427:3.1": "Pariplavantaṁ pathaviṁ, ",
  "ja427:3.2": "yadā tāta vijānahi; ",
  "ja427:3.3": "Sāgarena parikkhittaṁ, ",
  "ja427:3.4": "cakkaṁva parimaṇḍalaṁ; ",
  "ja427:3.5": "Tato tāta nivattassu, ",
  "ja427:3.6": "māssu etto paraṁ gami. ",
  "ja427:4.1": "Udapattosi vegena, ",
  "ja427:4.2": "balī pakkhī dijuttamo; ",
  "ja427:4.3": "Olokayanto vakkaṅgo, ",
  "ja427:4.4": "pabbatāni vanāni ca. ",
  "ja427:5.1": "Addassa pathaviṁ gijjho, ",
  "ja427:5.2": "yathāsāsi pitussutaṁ; ",
  "ja427:5.3": "Sāgarena parikkhittaṁ, ",
  "ja427:5.4": "cakkaṁva parimaṇḍalaṁ. ",
  "ja427:6.1": "Tañca so samatikkamma, ",
  "ja427:6.2": "paramevaccavattatha; ",
  "ja427:6.3": "Tañca vātasikhā tikkhā, ",
  "ja427:6.4": "accahāsi baliṁ dijaṁ. ",
  "ja427:7.1": "Nāsakkhātigato poso, ",
  "ja427:7.2": "punadeva nivattituṁ; ",
  "ja427:7.3": "Dijo byasanamāpādi, ",
  "ja427:7.4": "verambhānaṁ vasaṁ gato. ",
  "ja427:8.1": "Tassa puttā ca dārā ca, ",
  "ja427:8.2": "ye caññe anujīvino; ",
  "ja427:8.3": "Sabbe byasanamāpāduṁ, ",
  "ja427:8.4": "anovādakare dije. ",
  "ja427:9.1": "Evampi idha vuḍḍhānaṁ, ",
  "ja427:9.2": "yo vākyaṁ nāvabujjhati; ",
  "ja427:9.3": "Atisīmacaro ditto, ",
  "ja427:9.4": "gijjhovātītasāsano; ",
  "ja427:9.5": "Sa ve byasanaṁ pappoti, ",
  "ja427:9.6": "akatvā vuḍḍhasāsanan”ti. ",
  "ja427:10.1": "Gijjhajātakaṁ paṭhamaṁ. "
}