{
  "ja366:0.1": "Jātaka ",
  "ja366:0.2": "Pañcakanipāta ",
  "ja366:0.3": "Vaṇṇārohavagga ",
  "ja366:0.4": "6. Gumbiyajātaka ",
  "ja366:1.1": "“Madhuvaṇṇaṁ madhurasaṁ, ",
  "ja366:1.2": "madhugandhaṁ visaṁ ahu; ",
  "ja366:1.3": "Gumbiyo ghāsamesāno, ",
  "ja366:1.4": "araññe odahī visaṁ. ",
  "ja366:2.1": "Madhu iti maññamānā, ",
  "ja366:2.2": "ye taṁ visamakhādisuṁ; ",
  "ja366:2.3": "Tesaṁ taṁ kaṭukaṁ āsi, ",
  "ja366:2.4": "maraṇaṁ tenupāgamuṁ. ",
  "ja366:3.1": "Ye ca kho paṭisaṅkhāya, ",
  "ja366:3.2": "visaṁ taṁ parivajjayuṁ; ",
  "ja366:3.3": "Te āturesu sukhitā, ",
  "ja366:3.4": "ḍayhamānesu nibbutā. ",
  "ja366:4.1": "Evameva manussesu, ",
  "ja366:4.2": "visaṁ kāmā samohitā; ",
  "ja366:4.3": "Āmisaṁ bandhanañcetaṁ, ",
  "ja366:4.4": "maccuveso guhāsayo. ",
  "ja366:5.1": "Evameva ime kāme, ",
  "ja366:5.2": "āturā paricārike; ",
  "ja366:5.3": "Ye sadā parivajjenti, ",
  "ja366:5.4": "saṅgaṁ loke upaccagun”ti. ",
  "ja366:6.1": "Gumbiyajātakaṁ chaṭṭhaṁ. "
}