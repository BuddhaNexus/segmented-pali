{
  "anya-e1008n:6": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1008n:7": "Cāṇakyanītipāḷi",
  "anya-e1008n:8": "1.",
  "anya-e1008n:9": "Nānāsattho ddhataṃ vakkhe,",
  "anya-e1008n:10": "Rāja nīti samuccayaṃ;",
  "anya-e1008n:11": "Sabba bīja midaṃ satthaṃ,",
  "anya-e1008n:12": "Cāṇakyaṃ sārasaṅgahaṃ.",
  "anya-e1008n:13": "2.",
  "anya-e1008n:14": "Mūlasuttaṃ pavakkhāmi,",
  "anya-e1008n:15": "Cāṇakyena yathoditaṃ;",
  "anya-e1008n:16": "Yassa viññāna mattena,",
  "anya-e1008n:17": "Mūḷho bhavati paṇḍito.",
  "anya-e1008n:18": "3.",
  "anya-e1008n:19": "Viduttaṃ narapaccañca,",
  "anya-e1008n:20": "Nevatulyaṃ kudācanaṃ;",
  "anya-e1008n:21": "Sadese pujjate rājā,",
  "anya-e1008n:22": "Vidū sabbattha pujjate.",
  "anya-e1008n:23": "4.",
  "anya-e1008n:24": "Paṇḍite ca guṇā sabbe,",
  "anya-e1008n:25": "Mūḷhe dosā hi kevalaṃ;",
  "anya-e1008n:26": "Tasmā mūḷhasahassesu,",
  "anya-e1008n:27": "Pañño eko visisyate.",
  "anya-e1008n:28": "5.",
  "anya-e1008n:29": "Mātariva paradāresu,",
  "anya-e1008n:30": "Paradabbesu leṭṭuva;",
  "anya-e1008n:31": "Attaniva sabbabhūtesu,",
  "anya-e1008n:32": "Yo passati sapaṇḍito.",
  "anya-e1008n:33": "6.",
  "anya-e1008n:34": "Kiṃkulena visālena,",
  "anya-e1008n:35": "Guṇahīno tu yonaro;",
  "anya-e1008n:36": "Akulinopi satthañño,",
  "anya-e1008n:37": "Devatāhipi pujjate.",
  "anya-e1008n:38": "7.",
  "anya-e1008n:39": "Rūpayobbanasampannā,",
  "anya-e1008n:40": "Visālakulasambhavā;",
  "anya-e1008n:41": "Vijjāhīnā nasobhante,",
  "anya-e1008n:42": "Niggandhā iva kiṃsukā.",
  "anya-e1008n:43": "8.",
  "anya-e1008n:44": "Tārānaṃ bhūsaṇaṃ cando,",
  "anya-e1008n:45": "Nārīnaṃ bhūsaṇaṃ pati;",
  "anya-e1008n:46": "Puthabyā bhūsaṇaṃ rājā,",
  "anya-e1008n:47": "Vijjā sabbassa bhūsaṇaṃ.",
  "anya-e1008n:48": "9.",
  "anya-e1008n:49": "Mātā sattu pitā verī,",
  "anya-e1008n:50": "Yena bālo napāṭṭhito;",
  "anya-e1008n:51": "Na sobhate sabhāmajjhe,",
  "anya-e1008n:52": "Haṃsamajjhe bakoyathā.",
  "anya-e1008n:53": "10.",
  "anya-e1008n:54": "Varameko guṇī putto,",
  "anya-e1008n:55": "Na ca mūḷhasatehipi;",
  "anya-e1008n:56": "Eko cando tamo hanti,",
  "anya-e1008n:57": "Na ca tāragaṇehipi.",
  "anya-e1008n:58": "11.",
  "anya-e1008n:59": "Lālaye pañcavassāni,",
  "anya-e1008n:60": "Dasavassāni tālaye;",
  "anya-e1008n:61": "Patte tu soḷase vasse,",
  "anya-e1008n:62": "Puttaṃ mittaṃva ācare.",
  "anya-e1008n:63": "12.",
  "anya-e1008n:64": "Lālane bahavo dosā,",
  "anya-e1008n:65": "Tālane bahavo guṇā;",
  "anya-e1008n:66": "Tasmā puttañca sissañca,",
  "anya-e1008n:67": "Tālaye na tu lālaye.",
  "anya-e1008n:68": "13.",
  "anya-e1008n:69": "Ekenāpi suvakkhena,",
  "anya-e1008n:70": "Pupphitena sugandhinā;",
  "anya-e1008n:71": "Vāsitassa vanaṃ sabbaṃ,",
  "anya-e1008n:72": "Suputtena kulaṃyathā.",
  "anya-e1008n:73": "14.",
  "anya-e1008n:74": "Ekassāpi kuvakkhassa,",
  "anya-e1008n:75": "Koṭaraṭṭhena agginā;",
  "anya-e1008n:76": "Dayhate hi vanaṃ sabbaṃ,",
  "anya-e1008n:77": "Kuputtena kulaṃ yathā.",
  "anya-e1008n:78": "15.",
  "anya-e1008n:79": "Dūrato sobhate mūḷho,",
  "anya-e1008n:80": "Lambamāna paṭāvuto;",
  "anya-e1008n:81": "Tāvañca sobhate mūḷho,",
  "anya-e1008n:82": "Yāva kiñci nabhāsate.",
  "anya-e1008n:83": "16.",
  "anya-e1008n:84": "Visato amataṃ gāyhaṃ,",
  "anya-e1008n:85": "Amejjhāapi kañcanaṃ;",
  "anya-e1008n:86": "Nīcato uttamā vijjā,",
  "anya-e1008n:87": "Thīratnaṃ dukkulāapi.",
  "anya-e1008n:88": "17.",
  "anya-e1008n:89": "Ussave byasaneceva,",
  "anya-e1008n:90": "Dubbhikkhe sattuviggahe;",
  "anya-e1008n:91": "Rājadvāre susāneca,",
  "anya-e1008n:92": "Yo tiṭṭhati sabandhavo.",
  "anya-e1008n:93": "18.",
  "anya-e1008n:94": "Parokkhe kiccahantāraṃ,",
  "anya-e1008n:95": "Paccakkhe piyavādinaṃ;",
  "anya-e1008n:96": "Vajjaye tādisaṃ mittaṃ,",
  "anya-e1008n:97": "Visakumbhaṃ payomukhaṃ.",
  "anya-e1008n:98": "19.",
  "anya-e1008n:99": "Sakiṃ duṭṭhañca mittaṃ yo,",
  "anya-e1008n:100": "Puna sandhātu micchati;",
  "anya-e1008n:101": "Samaccu mupagaṇhāti,",
  "anya-e1008n:102": "Gabbha massatarī yathā.",
  "anya-e1008n:103": "20.",
  "anya-e1008n:104": "Na vissase avissatthaṃ,",
  "anya-e1008n:105": "Mittañcāpi na vissase;",
  "anya-e1008n:106": "Kadāci kupitaṃ mittaṃ,",
  "anya-e1008n:107": "Sabbadosaṃ pakāsaye.",
  "anya-e1008n:108": "21.",
  "anya-e1008n:109": "Jāniyā pesane bhacce,",
  "anya-e1008n:110": "Bandhave byasanāgame;",
  "anya-e1008n:111": "Mittañcā padikāleca,",
  "anya-e1008n:112": "Bhariyañca vibhavakkhaye.",
  "anya-e1008n:113": "22.",
  "anya-e1008n:114": "Upakāraggahitena,",
  "anya-e1008n:115": "Sattunāsattumuddhare;",
  "anya-e1008n:116": "Pādalaggaṃ karaṭṭhena,",
  "anya-e1008n:117": "Kaṇṭakeneva kaṇṭakaṃ.",
  "anya-e1008n:118": "23.",
  "anya-e1008n:119": "Na mittaṃ koci kassaci,",
  "anya-e1008n:120": "Na koci ripu kassaci;",
  "anya-e1008n:121": "Kāraṇena hi ñāyati,",
  "anya-e1008n:122": "Mittāni ca ripū tathā.",
  "anya-e1008n:123": "24.",
  "anya-e1008n:124": "Dujjano piyavādī ca,",
  "anya-e1008n:125": "Netaṃ vissāsakāraṇaṃ;",
  "anya-e1008n:126": "Madhu tiṭṭhati jivhagge,",
  "anya-e1008n:127": "Hadaye tu halāhalaṃ.",
  "anya-e1008n:128": "25.",
  "anya-e1008n:129": "Dujjano parihantabbo,",
  "anya-e1008n:130": "Vijjāyā laṅkatopi saṃ;",
  "anya-e1008n:131": "Maṇinā bhūsito sappo,",
  "anya-e1008n:132": "Kime so nabhayaṃ karo.",
  "anya-e1008n:133": "26.",
  "anya-e1008n:134": "Sappo kūro khalo kūro,",
  "anya-e1008n:135": "Sappā kūrataro khalo;",
  "anya-e1008n:136": "Manto sadhivaso sappo,",
  "anya-e1008n:137": "Khalo kena nivāyyate.",
  "anya-e1008n:138": "27.",
  "anya-e1008n:139": "Nakhīnañca nadīnañca,",
  "anya-e1008n:140": "Siṅgīnaṃ satthapāṇinaṃ;",
  "anya-e1008n:141": "Vissāso nevakātabbo,",
  "anya-e1008n:142": "Thīsu rājakulesu ca.",
  "anya-e1008n:143": "28.",
  "anya-e1008n:144": "Hatthī hatthasahassena,",
  "anya-e1008n:145": "Satahatthena vājino;",
  "anya-e1008n:146": "Siṅgino dasahatthena,",
  "anya-e1008n:147": "Ṭhānacāgena dujjano.",
  "anya-e1008n:148": "29.",
  "anya-e1008n:149": "Āpadatthaṃ dhanaṃ rakkhe,",
  "anya-e1008n:150": "Dāraṃ rakkhe dhanehipi;",
  "anya-e1008n:151": "Attānaṃ satataṃ rakkhe,",
  "anya-e1008n:152": "Dārehipi dhanehipi.",
  "anya-e1008n:153": "30.",
  "anya-e1008n:154": "Paradāraṃ paradabbaṃ,",
  "anya-e1008n:155": "Parivādaṃ parassa ca;",
  "anya-e1008n:156": "Parihāsaṃ guruṭṭhāne,",
  "anya-e1008n:157": "Cāpalyañca vivajjaye.",
  "anya-e1008n:158": "31.",
  "anya-e1008n:159": "Caje ekaṃ kulassatthe,",
  "anya-e1008n:160": "Gāmassatthe kulaṃ caje;",
  "anya-e1008n:161": "Gāma janapadassatthe,",
  "anya-e1008n:162": "Attatthe pathaviṃ caje.",
  "anya-e1008n:163": "32.",
  "anya-e1008n:164": "Calatyekena pādena,",
  "anya-e1008n:165": "Tiṭṭha tyekena buddhimā;",
  "anya-e1008n:166": "Nāsamikkhya paraṃ ṭhānaṃ,",
  "anya-e1008n:167": "Pubbamāyatanaṃ caje.",
  "anya-e1008n:168": "33.",
  "anya-e1008n:169": "Luddha matthena gaṇheyya,",
  "anya-e1008n:170": "Thaddha mañjalī kammunā;",
  "anya-e1008n:171": "Mūḷhaṃ chando nuvattena,",
  "anya-e1008n:172": "Tathā tathena paṇḍitaṃ.",
  "anya-e1008n:173": "34.",
  "anya-e1008n:174": "Atthanāsaṃ manotāpaṃ,",
  "anya-e1008n:175": "Gehe duccaritāni ca;",
  "anya-e1008n:176": "Vañcanañca pamāṇañca,",
  "anya-e1008n:177": "Matimā na pakāsaye.",
  "anya-e1008n:178": "35.",
  "anya-e1008n:179": "Dhanadhaññappayogesu,",
  "anya-e1008n:180": "Tathā vijjāgamesu ca;",
  "anya-e1008n:181": "Āhāre byavahāre ca,",
  "anya-e1008n:182": "Cattalajjo sadā bhave.",
  "anya-e1008n:183": "36.",
  "anya-e1008n:184": "Dhanino sotthiyo rājā,",
  "anya-e1008n:185": "Nadī vejjo tu pañcamo;",
  "anya-e1008n:186": "Pañca yatra navijjante,",
  "anya-e1008n:187": "Tatra vāsaṃ nakāraye.",
  "anya-e1008n:188": "37.",
  "anya-e1008n:189": "Yasmiṃdese na sammānaṃ,",
  "anya-e1008n:190": "Na pīti naca bandhavā;",
  "anya-e1008n:191": "Na ca vijjāgamo koci,",
  "anya-e1008n:192": "Taṃdesaṃ parivajjaye.",
  "anya-e1008n:193": "38.",
  "anya-e1008n:194": "Manasā cintitaṃ kammaṃ,",
  "anya-e1008n:195": "Vacasā napakāsaye;",
  "anya-e1008n:196": "Aññalakkhita kāriyassa,",
  "anya-e1008n:197": "Yato siddhi najāyate.",
  "anya-e1008n:198": "39.",
  "anya-e1008n:199": "Kudesañca kuvuttiñca,",
  "anya-e1008n:200": "Kubhariyaṃ kunadiṃ tathā;",
  "anya-e1008n:201": "Kudabbañca kubhojjañca,",
  "anya-e1008n:202": "Vajjaye tu vicakkhaṇo.",
  "anya-e1008n:203": "40.",
  "anya-e1008n:204": "Iṇasesoggi seso ca,",
  "anya-e1008n:205": "Byādhiseso tatheva ca;",
  "anya-e1008n:206": "Puna ca vaḍḍhate yasmā,",
  "anya-e1008n:207": "Tasmā sesaṃ nakāraye.",
  "anya-e1008n:208": "41.",
  "anya-e1008n:209": "Cintā jaro manussānaṃ,",
  "anya-e1008n:210": "Vatthānaṃ ātapo jaro;",
  "anya-e1008n:211": "Asobhagyaṃ jaro thīnaṃ,",
  "anya-e1008n:212": "Assānaṃ methunaṃ jaro.",
  "anya-e1008n:213": "42.",
  "anya-e1008n:214": "Atthi putto vase yassa,",
  "anya-e1008n:215": "Bhacco bhariyā tatheva ca;",
  "anya-e1008n:216": "Abhāve pyatisantoso,",
  "anya-e1008n:217": "Saggaṭṭho so mahītale.",
  "anya-e1008n:218": "43.",
  "anya-e1008n:219": "Duṭṭhā bhariyā saṭhaṃ mittaṃ,",
  "anya-e1008n:220": "Bhacco cuttaradāyako;",
  "anya-e1008n:221": "Sa sappeca gahe vāso,",
  "anya-e1008n:222": "Maccureva nasaṃsayo.",
  "anya-e1008n:223": "44.",
  "anya-e1008n:224": "Mātā yassa gehe natthi,",
  "anya-e1008n:225": "Bhariyācā piyavādinī;",
  "anya-e1008n:226": "Araññaṃ tena gantabbaṃ,",
  "anya-e1008n:227": "Yathā raññaṃ tathāgahaṃ.",
  "anya-e1008n:228": "45.",
  "anya-e1008n:229": "Iṇakattā pitā sattu,",
  "anya-e1008n:230": "Mātā ca byabhicārinī;",
  "anya-e1008n:231": "Bhariyā rūpavatī sattu,",
  "anya-e1008n:232": "Putto sattu apaṇḍito.",
  "anya-e1008n:233": "46.",
  "anya-e1008n:234": "Kokilānaṃ saro rūpaṃ,",
  "anya-e1008n:235": "Nārī rūpaṃ patibbatā;",
  "anya-e1008n:236": "Vijjā rūpaṃ kurūpānaṃ,",
  "anya-e1008n:237": "Khamā rūpaṃ tapassinaṃ.",
  "anya-e1008n:238": "47.",
  "anya-e1008n:239": "Avijjaṃ jīvanaṃ suññaṃ,",
  "anya-e1008n:240": "Disā suññā abandhavā;",
  "anya-e1008n:241": "Puttahīnaṃ gahaṃ suññaṃ,",
  "anya-e1008n:242": "Sabbasuññā daliddatā.",
  "anya-e1008n:243": "48.",
  "anya-e1008n:244": "Adātā vaṃsadosena,",
  "anya-e1008n:245": "Kammadosā daliddatā;",
  "anya-e1008n:246": "Ummādā mātudosena,",
  "anya-e1008n:247": "Pitudosena mūḷhatā.",
  "anya-e1008n:248": "49.",
  "anya-e1008n:249": "Guru aggi dvijādīnaṃ,",
  "anya-e1008n:250": "Vaṇṇānaṃ brāhmaṇo guru;",
  "anya-e1008n:251": "Pati reko gurutthīnaṃ,",
  "anya-e1008n:252": "Sabbassābhyāgato guru.",
  "anya-e1008n:253": "50.",
  "anya-e1008n:254": "Atidabbe hatā laṅkā,",
  "anya-e1008n:255": "Atimāne ca koravā;",
  "anya-e1008n:256": "Atidāne balībaddho,",
  "anya-e1008n:257": "Sabbamaccanta gahitaṃ.",
  "anya-e1008n:258": "51.",
  "anya-e1008n:259": "Vatthahīno tvalaṅkāro,",
  "anya-e1008n:260": "Ghatahīnañca bhojanaṃ;",
  "anya-e1008n:261": "Thanahīnā ca yānārī,",
  "anya-e1008n:262": "Vijjāhīnañca jīvanaṃ.",
  "anya-e1008n:263": "52.",
  "anya-e1008n:264": "Bhojjaṃ bhojanasatti ca,",
  "anya-e1008n:265": "Ratisatti varā thiyo;",
  "anya-e1008n:266": "Vibhavo dānasatti ca,",
  "anya-e1008n:267": "Nāppassa tapaso phalaṃ.",
  "anya-e1008n:268": "53.",
  "anya-e1008n:269": "Puttappayojanā dārā,",
  "anya-e1008n:270": "Putto piṇḍappayojano;",
  "anya-e1008n:271": "Hitappayojanaṃ mittaṃ,",
  "anya-e1008n:272": "Dhanaṃ sabbappayojanaṃ.",
  "anya-e1008n:273": "54.",
  "anya-e1008n:274": "Dullabhaṃ pākatikaṃ vākyaṃ,",
  "anya-e1008n:275": "Dullabho khemakaro suto;",
  "anya-e1008n:276": "Dullabhā sadisī jāyā,",
  "anya-e1008n:277": "Dullabho sajano piyo.",
  "anya-e1008n:278": "55.",
  "anya-e1008n:279": "Selesele namāṇikkaṃ,",
  "anya-e1008n:280": "Muttikaṃ na gajegaje;",
  "anya-e1008n:281": "Sādhavo na hi sabbatra,",
  "anya-e1008n:282": "Candanaṃ na vanevane.",
  "anya-e1008n:283": "56.",
  "anya-e1008n:284": "Asoco niddhano pañño,",
  "anya-e1008n:285": "Asoco paṇḍitabandhavo;",
  "anya-e1008n:286": "Asocā vidhavā nārī,",
  "anya-e1008n:287": "Puttanatta patiṭṭhitā.",
  "anya-e1008n:288": "57.",
  "anya-e1008n:289": "Avijjo puriso soco,",
  "anya-e1008n:290": "Socaṃ methuna mappajaṃ;",
  "anya-e1008n:291": "Nirāhārā pajā socā,",
  "anya-e1008n:292": "Socaṃ rajja marājakaṃ.",
  "anya-e1008n:293": "58.",
  "anya-e1008n:294": "Kulehi saha sampakkaṃ,",
  "anya-e1008n:295": "Paṇḍitehi ca mittataṃ;",
  "anya-e1008n:296": "Ñātībhi ca samaṃ melaṃ,",
  "anya-e1008n:297": "Kubbāno navinassati.",
  "anya-e1008n:298": "59.",
  "anya-e1008n:299": "Kaṭṭhā vutti parādhinā,",
  "anya-e1008n:300": "Kaṭṭho vāso nirāsayo;",
  "anya-e1008n:301": "Niddhano byavasāyo ca,",
  "anya-e1008n:302": "Sabbakaṭṭhā daliddatā.",
  "anya-e1008n:303": "60.",
  "anya-e1008n:304": "Takkarassa kuto dhammo,",
  "anya-e1008n:305": "Dujjanassa kuto khamā;",
  "anya-e1008n:306": "Vesiyā ca kuto sneho,",
  "anya-e1008n:307": "Kuto saccañca kāminaṃ.",
  "anya-e1008n:308": "61.",
  "anya-e1008n:309": "Pesitassa kuto mānaṃ,",
  "anya-e1008n:310": "Kopanassa kuto sukhaṃ;",
  "anya-e1008n:311": "Thīnaṃ kuto satittañca,",
  "anya-e1008n:312": "Kuto mettī khalassa ca.",
  "anya-e1008n:313": "62.",
  "anya-e1008n:314": "Dubbalassa balaṃ rājā,",
  "anya-e1008n:315": "Bālānaṃ rodanaṃ balaṃ;",
  "anya-e1008n:316": "Balaṃmūḷhassa monittaṃ,",
  "anya-e1008n:317": "Corānaṃ atathaṃ balaṃ.",
  "anya-e1008n:318": "63.",
  "anya-e1008n:319": "Yo dhuvāni pariccajja,",
  "anya-e1008n:320": "Adhuvaṃ parisevati;",
  "anya-e1008n:321": "Dhuvāni tassa nassanti,",
  "anya-e1008n:322": "Adhuvaṃ naṭṭhameva ca.",
  "anya-e1008n:323": "64.",
  "anya-e1008n:324": "Sukkaṃ maṃsaṃ thiyo vuddhā,",
  "anya-e1008n:325": "Bālakka taruṇaṃ dadhi;",
  "anya-e1008n:326": "Pabhāte methunaṃ niddā,",
  "anya-e1008n:327": "Sajju pāṇaharāni cha;",
  "anya-e1008n:328": "65.",
  "anya-e1008n:329": "Sajju maṃsaṃ navannañca,",
  "anya-e1008n:330": "Bālā thī khīrabhojanaṃ;",
  "anya-e1008n:331": "Ghatamuṇhodakañceva,",
  "anya-e1008n:332": "Sajju pāṇakarāni cha.",
  "anya-e1008n:333": "66.",
  "anya-e1008n:334": "Sīhādekaṃ bakādekaṃ,",
  "anya-e1008n:335": "Cha sunā tīṇi gadrabhā;",
  "anya-e1008n:336": "Vāyasā catu sikkhetha,",
  "anya-e1008n:337": "Cattāri kukkuṭādapi.",
  "anya-e1008n:338": "67.",
  "anya-e1008n:339": "Pabhūtamappakiccaṃ vā,",
  "anya-e1008n:340": "Yonaro kattumicchati;",
  "anya-e1008n:341": "Saṃyatanena kattabbaṃ,",
  "anya-e1008n:342": "Sīhādekaṃ pakittitaṃ.",
  "anya-e1008n:343": "68.",
  "anya-e1008n:344": "Sabbindriyāni saṃyama,",
  "anya-e1008n:345": "Bakova patito jano;",
  "anya-e1008n:346": "Kāladesopapannāni,",
  "anya-e1008n:347": "Sabbakiccāni sādhaye.",
  "anya-e1008n:348": "69.",
  "anya-e1008n:349": "Bahvāsī sāppasantuṭṭho,",
  "anya-e1008n:350": "Suniddo sīghacetano;",
  "anya-e1008n:351": "Pabhubhatto ca sūro ca,",
  "anya-e1008n:352": "Ñātabbā cha sunā guṇā.",
  "anya-e1008n:353": "70.",
  "anya-e1008n:354": "Avissāmaṃ vahe bhāraṃ,",
  "anya-e1008n:355": "Sītuṇhañca navindati;",
  "anya-e1008n:356": "Sa santoso tathā niccaṃ,",
  "anya-e1008n:357": "Tīṇi sikkhetha gadrabhā.",
  "anya-e1008n:358": "71.",
  "anya-e1008n:359": "Guḷhamethunadhammañca,",
  "anya-e1008n:360": "Kālekāle ca saṅgahaṃ;",
  "anya-e1008n:361": "Appamādamanālasyaṃ,",
  "anya-e1008n:362": "Catu sikkhetha vāyasā.",
  "anya-e1008n:363": "72.",
  "anya-e1008n:364": "Yuddhañca pātaruṭṭhānaṃ,",
  "anya-e1008n:365": "Bhojanaṃ saha bandhuhi;",
  "anya-e1008n:366": "Thiyaṃ āpadaggataṃ rakkhe,",
  "anya-e1008n:367": "Catu sikkhetha kukkuṭā.",
  "anya-e1008n:368": "73.",
  "anya-e1008n:369": "Kotibhāro samatthānaṃ,",
  "anya-e1008n:370": "Kiṃdūraṃ byavasāyinaṃ;",
  "anya-e1008n:371": "Ko videso savijjānaṃ,",
  "anya-e1008n:372": "Ko paro piyavādinaṃ.",
  "anya-e1008n:373": "74.",
  "anya-e1008n:374": "Bhayassa kathito pantho,",
  "anya-e1008n:375": "Indriyānamasaṃyamo;",
  "anya-e1008n:376": "Tajjayo sampadāmaggo,",
  "anya-e1008n:377": "Yeniṭṭhaṃ tena gamyate.",
  "anya-e1008n:378": "75.",
  "anya-e1008n:379": "Na ca vijjāsamo bandhu,",
  "anya-e1008n:380": "Na ca byādhisamo ripu;",
  "anya-e1008n:381": "Nacāpaccasamo sneho,",
  "anya-e1008n:382": "Na ca devā paraṃ balaṃ.",
  "anya-e1008n:383": "76.",
  "anya-e1008n:384": "Samuddāvaraṇā bhūmi,",
  "anya-e1008n:385": "Pākārāvaraṇaṃ gahaṃ;",
  "anya-e1008n:386": "Narindāvaraṇā desā,",
  "anya-e1008n:387": "Cārittāvaraṇā thiyo.",
  "anya-e1008n:388": "77.",
  "anya-e1008n:389": "Ghatakumbhasamā nārī,",
  "anya-e1008n:390": "Tattaṅgāra samo pumā;",
  "anya-e1008n:391": "Tasmā ghatañca aggiñca,",
  "anya-e1008n:392": "Nekatra thapaye budho.",
  "anya-e1008n:393": "78.",
  "anya-e1008n:394": "Āhāro dviguṇo thīnaṃ,",
  "anya-e1008n:395": "Buddhi tāsaṃ catugguṇo;",
  "anya-e1008n:396": "Chaguṇo byavasāyo ca,",
  "anya-e1008n:397": "Kāmocaṭṭhaguṇo mato.",
  "anya-e1008n:398": "79.",
  "anya-e1008n:399": "Jiṇṇamannaṃ pasaṃseyya,",
  "anya-e1008n:400": "Bhariyaṃ gatayobbanaṃ;",
  "anya-e1008n:401": "Raṇā paccāgataṃ sūraṃ,",
  "anya-e1008n:402": "Sassañca gehamāgataṃ.",
  "anya-e1008n:403": "80.",
  "anya-e1008n:404": "Asantuṭṭhā dvijā naṭṭhā,",
  "anya-e1008n:405": "Santuṭṭhāiva pāthivā;",
  "anya-e1008n:406": "Salajjā gaṇikā naṭṭhā,",
  "anya-e1008n:407": "Nillajjā ca kulitthiyo.",
  "anya-e1008n:408": "81.",
  "anya-e1008n:409": "Avaṃsapatito rājā,",
  "anya-e1008n:410": "Mūḷhaputto ca paṇḍito;",
  "anya-e1008n:411": "Adhanena dhanaṃ pāpya,",
  "anya-e1008n:412": "Tiṇaṃva maññate janaṃ.",
  "anya-e1008n:413": "82.",
  "anya-e1008n:414": "Brahmahāpi naro pujjo,",
  "anya-e1008n:415": "Yassatthi vipulaṃ dhanaṃ;",
  "anya-e1008n:416": "Sasino tulyavaṃsopi,",
  "anya-e1008n:417": "Niddhano paribhūyate.",
  "anya-e1008n:418": "83.",
  "anya-e1008n:419": "Potthakaṭṭhā tu yāvijjā,",
  "anya-e1008n:420": "Parahatthagataṃ dhanaṃ;",
  "anya-e1008n:421": "Kiccakāle samuppanne,",
  "anya-e1008n:422": "Na sāvijjā na taddhanaṃ.",
  "anya-e1008n:423": "84.",
  "anya-e1008n:424": "Pādapānaṃ bhayaṃ vātā,",
  "anya-e1008n:425": "Padmānaṃ sisirā bhayaṃ;",
  "anya-e1008n:426": "Pabbatānaṃ vajīramhā,",
  "anya-e1008n:427": "Sādhūnaṃ dujjanā bhayaṃ.",
  "anya-e1008n:428": "85.",
  "anya-e1008n:429": "Paññe niyujjamāne tu,",
  "anya-e1008n:430": "Santi rañño tayoguṇā;",
  "anya-e1008n:431": "Yaso sagganivāso ca,",
  "anya-e1008n:432": "Vipulo ca dhanāgamo.",
  "anya-e1008n:433": "86.",
  "anya-e1008n:434": "Mūḷhe niyujjamānetu,",
  "anya-e1008n:435": "Khattiyassāguṇā tayo;",
  "anya-e1008n:436": "Ayaso catthanāso ca,",
  "anya-e1008n:437": "Narake gamanaṃ tathā.",
  "anya-e1008n:438": "87.",
  "anya-e1008n:439": "Bahūmūḷhasaṅghātehi,",
  "anya-e1008n:440": "Aññoññapasuvuttibhi;",
  "anya-e1008n:441": "Pacchādyante guṇā sabbe,",
  "anya-e1008n:442": "Meghehiva divākaro.",
  "anya-e1008n:443": "88.",
  "anya-e1008n:444": "Yassa khettaṃ nadītīre,",
  "anya-e1008n:445": "Bhariyāpi parappiyā;",
  "anya-e1008n:446": "Puttassa vinayo natthi,",
  "anya-e1008n:447": "Maccureva nasaṃsayo.",
  "anya-e1008n:448": "89.",
  "anya-e1008n:449": "Asambhābyaṃ navattabbaṃ,",
  "anya-e1008n:450": "Paccakkhamapi dissate;",
  "anya-e1008n:451": "Silā tarati pānīyaṃ,",
  "anya-e1008n:452": "Gītaṃ gāyati vānaro.",
  "anya-e1008n:453": "90.",
  "anya-e1008n:454": "Subhikkhaṃ kasake niccaṃ,",
  "anya-e1008n:455": "Niccaṃ sukha marogike;",
  "anya-e1008n:456": "Bhariyā bhattu piyā yassa,",
  "anya-e1008n:457": "Tassa niccossavaṃ gahaṃ.",
  "anya-e1008n:458": "91.",
  "anya-e1008n:459": "Helassa kammanāsāya,",
  "anya-e1008n:460": "Buddhināsāya niddhanaṃ;",
  "anya-e1008n:461": "Yācanā mānanāsāya,",
  "anya-e1008n:462": "Kulanāsāya bhojanaṃ.",
  "anya-e1008n:463": "92.",
  "anya-e1008n:464": "Sevitabbo mahāvakkho,",
  "anya-e1008n:465": "Phalacchāyā samanvito;",
  "anya-e1008n:466": "Yadi devā phalaṃ natthi,",
  "anya-e1008n:467": "Chāyā kena nivāraye.",
  "anya-e1008n:468": "93.",
  "anya-e1008n:469": "Paṭhame najjitā vijjā,",
  "anya-e1008n:470": "Dutīye najjitaṃ dhanaṃ;",
  "anya-e1008n:471": "Tatīye najjitaṃ puññaṃ,",
  "anya-e1008n:472": "Catutthe kiṃkarissati.",
  "anya-e1008n:473": "94.",
  "anya-e1008n:474": "Nadīkūleca ye vakkhā,",
  "anya-e1008n:475": "Parahatthagataṃ dhanaṃ;",
  "anya-e1008n:476": "Kiccaṃ thīgocaraṃ yassa,",
  "anya-e1008n:477": "Sabbaṃ taṃ viphalaṃ bhave.",
  "anya-e1008n:478": "95.",
  "anya-e1008n:479": "Kudesamāsajja kutotthasañcayo,",
  "anya-e1008n:480": "Kuputtamāsajja kuto jalañjalī;",
  "anya-e1008n:481": "Kugehiniṃ pāpya gahe kuto sukhaṃ,",
  "anya-e1008n:482": "Kusissamajjhāpayato kuto yaso.",
  "anya-e1008n:483": "96.",
  "anya-e1008n:484": "Kūpodakaṃ vaṭacchāyā,",
  "anya-e1008n:485": "Sāmā thīciṭṭhakālayaṃ;",
  "anya-e1008n:486": "Sītakāle bhave uṇhaṃ,",
  "anya-e1008n:487": "Gimhakāle ca sītalaṃ.",
  "anya-e1008n:488": "97.",
  "anya-e1008n:489": "Visaṃ caṅkamanaṃ rattiṃ,",
  "anya-e1008n:490": "Visaṃ raññonukulatā;",
  "anya-e1008n:491": "Visaṃ thīpi aññāsattā,",
  "anya-e1008n:492": "Visaṃ byādhi avekkhito.",
  "anya-e1008n:493": "98.",
  "anya-e1008n:494": "Duradhītā visaṃ vijjā,",
  "anya-e1008n:495": "Ajiṇṇe bhojanaṃ visaṃ;",
  "anya-e1008n:496": "Visaṃ goṭṭhī daliddassa,",
  "anya-e1008n:497": "Vuddhassa taruṇī visaṃ.",
  "anya-e1008n:498": "99.",
  "anya-e1008n:499": "Padose nihato pantho,",
  "anya-e1008n:500": "Patitā nihatā thiyo;",
  "anya-e1008n:501": "Appabījaṃ hataṃ khettaṃ,",
  "anya-e1008n:502": "Bhaccadosā hato pabhū.",
  "anya-e1008n:503": "100.",
  "anya-e1008n:504": "Hatamasottiyaṃ saddhaṃ,",
  "anya-e1008n:505": "Hato yañño tvadakkhiṇo;",
  "anya-e1008n:506": "Hatā rūpavatī vañjhā,",
  "anya-e1008n:507": "Hataṃ senamanāyakaṃ.",
  "anya-e1008n:508": "101.",
  "anya-e1008n:509": "Vedavedaṅga tattañño,",
  "anya-e1008n:510": "Japahomaparāyano;",
  "anya-e1008n:511": "Āsīvādavacoyutto,",
  "anya-e1008n:512": "Esa rājapurohito.",
  "anya-e1008n:513": "102.",
  "anya-e1008n:514": "Kulasīlaguṇopeto,",
  "anya-e1008n:515": "Sabbadhammaparāyano;",
  "anya-e1008n:516": "Pavīṇo pesanādyakkho,",
  "anya-e1008n:517": "Dhammādyakkho vidhīyate.",
  "anya-e1008n:518": "103.",
  "anya-e1008n:519": "Ayubbedakatābhyāso,",
  "anya-e1008n:520": "Sabbesaṃ piyadassano;",
  "anya-e1008n:521": "Ariyasīlaguṇopeto,",
  "anya-e1008n:522": "Esa vajjo vidhīyate.",
  "anya-e1008n:523": "104.",
  "anya-e1008n:524": "Sakiṃdutta gahitattho,",
  "anya-e1008n:525": "Lahuhattho jitakkharo;",
  "anya-e1008n:526": "Sabbasattha samālokī,",
  "anya-e1008n:527": "Pakaṭṭho nāma lekhako.",
  "anya-e1008n:528": "105.",
  "anya-e1008n:529": "Samattanītisattañño,",
  "anya-e1008n:530": "Vāhane pūjitassamo;",
  "anya-e1008n:531": "Sūravīraguṇopeto,",
  "anya-e1008n:532": "Senādhyakkho vidhīyate.",
  "anya-e1008n:533": "106.",
  "anya-e1008n:534": "Sucī vākyapaṭuppañño,",
  "anya-e1008n:535": "Paracittopalakkhako;",
  "anya-e1008n:536": "Dhīro yathāttha vādī ca,",
  "anya-e1008n:537": "Esa dūto vidhīyate.",
  "anya-e1008n:538": "107.",
  "anya-e1008n:539": "Puttanatta guṇopeto,",
  "anya-e1008n:540": "Satthañño piṭṭhapācako;",
  "anya-e1008n:541": "Sūro ca kathinoceva,",
  "anya-e1008n:542": "Sūpakāro sa uccate.",
  "anya-e1008n:543": "108.",
  "anya-e1008n:544": "Iṅgitā kāratattañño,",
  "anya-e1008n:545": "Balavā piyadassano;",
  "anya-e1008n:546": "Appamādī sadā dakkho,",
  "anya-e1008n:547": "Patihāro sa uccate.",
  "anya-e1008n:548": "109.",
  "anya-e1008n:549": "Yassa natthi sayaṃ paññā,",
  "anya-e1008n:550": "Satthaṃ tassa karoti kiṃ;",
  "anya-e1008n:551": "Locanehi vihīnassa,",
  "anya-e1008n:552": "Dappaṇo kiṃkarissati.",
  "anya-e1008n:553": "110.",
  "anya-e1008n:554": "Kiṃkarissanti vattāro,",
  "anya-e1008n:555": "Sotaṃ yattha navijjate;",
  "anya-e1008n:556": "Naggakapaṇake dese,",
  "anya-e1008n:557": "Rajaṇo kiṃkarissati."
}