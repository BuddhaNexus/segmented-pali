{
  "anya-e1008n:0": " Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1008n:1": "Cāṇakyanītipāḷi",
  "anya-e1008n:2": "1.",
  "anya-e1008n:3": " Nānāsattho ddhataṁ vakkhe,",
  "anya-e1008n:4": "Rāja nīti samuccayaṁ;",
  "anya-e1008n:5": "Sabba bīja midaṁ satthaṁ,",
  "anya-e1008n:6": "Cāṇakyaṁ sārasaṅgahaṁ.",
  "anya-e1008n:7": "2.",
  "anya-e1008n:8": " Mūlasuttaṁ pavakkhāmi,",
  "anya-e1008n:9": "Cāṇakyena yathoditaṁ;",
  "anya-e1008n:10": "Yassa viññāna mattena,",
  "anya-e1008n:11": "Mūḷho bhavati paṇḍito.",
  "anya-e1008n:12": "3.",
  "anya-e1008n:13": " Viduttaṁ narapaccañca,",
  "anya-e1008n:14": "Nevatulyaṁ kudācanaṁ;",
  "anya-e1008n:15": "Sadese pujjate rājā,",
  "anya-e1008n:16": "Vidū sabbattha pujjate.",
  "anya-e1008n:17": "4.",
  "anya-e1008n:18": " Paṇḍite ca guṇā sabbe,",
  "anya-e1008n:19": "Mūḷhe dosā hi kevalaṁ;",
  "anya-e1008n:20": "Tasmā mūḷhasahassesu,",
  "anya-e1008n:21": "Pañño eko visisyate.",
  "anya-e1008n:22": "5.",
  "anya-e1008n:23": " Mātariva paradāresu,",
  "anya-e1008n:24": "Paradabbesu leṭṭuva;",
  "anya-e1008n:25": "Attaniva sabbabhūtesu,",
  "anya-e1008n:26": "Yo passati sapaṇḍito.",
  "anya-e1008n:27": "6.",
  "anya-e1008n:28": " Kiṁkulena visālena,",
  "anya-e1008n:29": "Guṇahīno tu yonaro;",
  "anya-e1008n:30": "Akulinopi satthañño,",
  "anya-e1008n:31": "Devatāhipi pujjate.",
  "anya-e1008n:32": "7.",
  "anya-e1008n:33": " Rūpayobbanasampannā ,",
  "anya-e1008n:34": "Visālakulasambhavā;",
  "anya-e1008n:35": "Vijjāhīnā nasobhante,",
  "anya-e1008n:36": "Niggandhā iva kiṁsukā.",
  "anya-e1008n:37": "8.",
  "anya-e1008n:38": " Tārānaṁ bhūsaṇaṁ cando,",
  "anya-e1008n:39": "Nārīnaṁ bhūsaṇaṁ pati;",
  "anya-e1008n:40": "Puthabyā bhūsaṇaṁ rājā,",
  "anya-e1008n:41": "Vijjā sabbassa bhūsaṇaṁ.",
  "anya-e1008n:42": "9.",
  "anya-e1008n:43": " Mātā sattu pitā verī,",
  "anya-e1008n:44": "Yena bālo napāṭṭhito;",
  "anya-e1008n:45": "Na sobhate sabhāmajjhe,",
  "anya-e1008n:46": "Haṁsamajjhe bakoyathā.",
  "anya-e1008n:47": "10.",
  "anya-e1008n:48": " Varameko guṇī putto,",
  "anya-e1008n:49": "Na ca mūḷhasatehipi;",
  "anya-e1008n:50": "Eko cando tamo hanti,",
  "anya-e1008n:51": "Na ca tāragaṇehipi.",
  "anya-e1008n:52": "11.",
  "anya-e1008n:53": " Lālaye pañcavassāni,",
  "anya-e1008n:54": "Dasavassāni tālaye;",
  "anya-e1008n:55": "Patte tu soḷase vasse,",
  "anya-e1008n:56": "Puttaṁ mittaṁva ācare.",
  "anya-e1008n:57": "12.",
  "anya-e1008n:58": " Lālane bahavo dosā,",
  "anya-e1008n:59": "Tālane bahavo guṇā;",
  "anya-e1008n:60": "Tasmā puttañca sissañca,",
  "anya-e1008n:61": "Tālaye na tu lālaye.",
  "anya-e1008n:62": "13.",
  "anya-e1008n:63": " Ekenāpi suvakkhena,",
  "anya-e1008n:64": "Pupphitena sugandhinā;",
  "anya-e1008n:65": "Vāsitassa vanaṁ sabbaṁ,",
  "anya-e1008n:66": "Suputtena kulaṁyathā.",
  "anya-e1008n:67": "14.",
  "anya-e1008n:68": " Ekassāpi kuvakkhassa,",
  "anya-e1008n:69": "Koṭaraṭṭhena agginā;",
  "anya-e1008n:70": "Dayhate hi vanaṁ sabbaṁ,",
  "anya-e1008n:71": "Kuputtena kulaṁ yathā.",
  "anya-e1008n:72": "15.",
  "anya-e1008n:73": " Dūrato sobhate mūḷho,",
  "anya-e1008n:74": "Lambamāna paṭāvuto;",
  "anya-e1008n:75": "Tāvañca sobhate mūḷho,",
  "anya-e1008n:76": "Yāva kiñci nabhāsate.",
  "anya-e1008n:77": "16.",
  "anya-e1008n:78": " Visato amataṁ gāyhaṁ,",
  "anya-e1008n:79": "Amejjhāapi kañcanaṁ;",
  "anya-e1008n:80": "Nīcato uttamā vijjā,",
  "anya-e1008n:81": "Thīratnaṁ dukkulāapi.",
  "anya-e1008n:82": "17.",
  "anya-e1008n:83": " Ussave byasaneceva,",
  "anya-e1008n:84": "Dubbhikkhe sattuviggahe;",
  "anya-e1008n:85": "Rājadvāre susāneca,",
  "anya-e1008n:86": "Yo tiṭṭhati sabandhavo.",
  "anya-e1008n:87": "18.",
  "anya-e1008n:88": " Parokkhe kiccahantāraṁ,",
  "anya-e1008n:89": "Paccakkhe piyavādinaṁ;",
  "anya-e1008n:90": "Vajjaye tādisaṁ mittaṁ,",
  "anya-e1008n:91": "Visakumbhaṁ payomukhaṁ.",
  "anya-e1008n:92": "19.",
  "anya-e1008n:93": " Sakiṁ duṭṭhañca mittaṁ yo,",
  "anya-e1008n:94": "Puna sandhātu micchati;",
  "anya-e1008n:95": "Samaccu mupagaṇhāti,",
  "anya-e1008n:96": "Gabbha massatarī yathā.",
  "anya-e1008n:97": "20.",
  "anya-e1008n:98": " Na vissase avissatthaṁ,",
  "anya-e1008n:99": "Mittañcāpi na vissase;",
  "anya-e1008n:100": "Kadāci kupitaṁ mittaṁ,",
  "anya-e1008n:101": "Sabbadosaṁ pakāsaye.",
  "anya-e1008n:102": "21.",
  "anya-e1008n:103": " Jāniyā pesane bhacce,",
  "anya-e1008n:104": "Bandhave byasanāgame;",
  "anya-e1008n:105": "Mittañcā padikāleca,",
  "anya-e1008n:106": "Bhariyañca vibhavakkhaye.",
  "anya-e1008n:107": "22.",
  "anya-e1008n:108": " Upakāraggahitena,",
  "anya-e1008n:109": "Sattunāsattumuddhare;",
  "anya-e1008n:110": "Pādalaggaṁ karaṭṭhena,",
  "anya-e1008n:111": "Kaṇṭakeneva kaṇṭakaṁ.",
  "anya-e1008n:112": "23.",
  "anya-e1008n:113": " Na mittaṁ koci kassaci,",
  "anya-e1008n:114": "Na koci ripu kassaci;",
  "anya-e1008n:115": "Kāraṇena hi ñāyati,",
  "anya-e1008n:116": "Mittāni ca ripū tathā.",
  "anya-e1008n:117": "24.",
  "anya-e1008n:118": " Dujjano piyavādī ca,",
  "anya-e1008n:119": "Netaṁ vissāsakāraṇaṁ;",
  "anya-e1008n:120": "Madhu tiṭṭhati jivhagge,",
  "anya-e1008n:121": "Hadaye tu halāhalaṁ.",
  "anya-e1008n:122": "25.",
  "anya-e1008n:123": " Dujjano parihantabbo,",
  "anya-e1008n:124": "Vijjāyā laṅkatopi saṁ;",
  "anya-e1008n:125": "Maṇinā bhūsito sappo,",
  "anya-e1008n:126": "Kime so nabhayaṁ karo.",
  "anya-e1008n:127": "26.",
  "anya-e1008n:128": " Sappo kūro khalo kūro,",
  "anya-e1008n:129": "Sappā kūrataro khalo;",
  "anya-e1008n:130": "Manto sadhivaso sappo,",
  "anya-e1008n:131": "Khalo kena nivāyyate.",
  "anya-e1008n:132": "27.",
  "anya-e1008n:133": " Nakhīnañca nadīnañca,",
  "anya-e1008n:134": "Siṅgīnaṁ satthapāṇinaṁ;",
  "anya-e1008n:135": "Vissāso nevakātabbo,",
  "anya-e1008n:136": "Thīsu rājakulesu ca.",
  "anya-e1008n:137": "28.",
  "anya-e1008n:138": " Hatthī hatthasahassena,",
  "anya-e1008n:139": "Satahatthena vājino;",
  "anya-e1008n:140": "Siṅgino dasahatthena,",
  "anya-e1008n:141": "Ṭhānacāgena dujjano.",
  "anya-e1008n:142": "29.",
  "anya-e1008n:143": " Āpadatthaṁ dhanaṁ rakkhe,",
  "anya-e1008n:144": "Dāraṁ rakkhe dhanehipi;",
  "anya-e1008n:145": "Attānaṁ satataṁ rakkhe,",
  "anya-e1008n:146": "Dārehipi dhanehipi.",
  "anya-e1008n:147": "30.",
  "anya-e1008n:148": " Paradāraṁ paradabbaṁ,",
  "anya-e1008n:149": "Parivādaṁ parassa ca;",
  "anya-e1008n:150": "Parihāsaṁ guruṭṭhāne,",
  "anya-e1008n:151": "Cāpalyañca vivajjaye.",
  "anya-e1008n:152": "31.",
  "anya-e1008n:153": " Caje ekaṁ kulassatthe,",
  "anya-e1008n:154": "Gāmassatthe kulaṁ caje;",
  "anya-e1008n:155": "Gāma janapadassatthe,",
  "anya-e1008n:156": "Attatthe pathaviṁ caje.",
  "anya-e1008n:157": "32.",
  "anya-e1008n:158": " Calatyekena pādena,",
  "anya-e1008n:159": "Tiṭṭha tyekena buddhimā;",
  "anya-e1008n:160": "Nāsamikkhya paraṁ ṭhānaṁ,",
  "anya-e1008n:161": "Pubbamāyatanaṁ caje.",
  "anya-e1008n:162": "33.",
  "anya-e1008n:163": " Luddha matthena gaṇheyya,",
  "anya-e1008n:164": "Thaddha mañjalī kammunā;",
  "anya-e1008n:165": "Mūḷhaṁ chando nuvattena,",
  "anya-e1008n:166": "Tathā tathena paṇḍitaṁ.",
  "anya-e1008n:167": "34.",
  "anya-e1008n:168": " Atthanāsaṁ manotāpaṁ,",
  "anya-e1008n:169": "Gehe duccaritāni ca;",
  "anya-e1008n:170": "Vañcanañca pamāṇañca,",
  "anya-e1008n:171": "Matimā na pakāsaye.",
  "anya-e1008n:172": "35.",
  "anya-e1008n:173": " Dhanadhaññappayogesu ,",
  "anya-e1008n:174": "Tathā vijjāgamesu ca;",
  "anya-e1008n:175": "Āhāre byavahāre ca,",
  "anya-e1008n:176": "Cattalajjo sadā bhave.",
  "anya-e1008n:177": "36.",
  "anya-e1008n:178": " Dhanino sotthiyo rājā,",
  "anya-e1008n:179": "Nadī vejjo tu pañcamo;",
  "anya-e1008n:180": "Pañca yatra navijjante,",
  "anya-e1008n:181": "Tatra vāsaṁ nakāraye.",
  "anya-e1008n:182": "37.",
  "anya-e1008n:183": " Yasmiṁdese na sammānaṁ,",
  "anya-e1008n:184": "Na pīti naca bandhavā;",
  "anya-e1008n:185": "Na ca vijjāgamo koci,",
  "anya-e1008n:186": "Taṁdesaṁ parivajjaye.",
  "anya-e1008n:187": "38.",
  "anya-e1008n:188": " Manasā cintitaṁ kammaṁ,",
  "anya-e1008n:189": "Vacasā napakāsaye;",
  "anya-e1008n:190": "Aññalakkhita kāriyassa,",
  "anya-e1008n:191": "Yato siddhi najāyate.",
  "anya-e1008n:192": "39.",
  "anya-e1008n:193": " Kudesañca kuvuttiñca,",
  "anya-e1008n:194": "Kubhariyaṁ kunadiṁ tathā;",
  "anya-e1008n:195": "Kudabbañca kubhojjañca,",
  "anya-e1008n:196": "Vajjaye tu vicakkhaṇo.",
  "anya-e1008n:197": "40.",
  "anya-e1008n:198": " Iṇasesoggi seso ca,",
  "anya-e1008n:199": "Byādhiseso tatheva ca;",
  "anya-e1008n:200": "Puna ca vaḍḍhate yasmā,",
  "anya-e1008n:201": "Tasmā sesaṁ nakāraye.",
  "anya-e1008n:202": "41.",
  "anya-e1008n:203": " Cintā jaro manussānaṁ,",
  "anya-e1008n:204": "Vatthānaṁ ātapo jaro;",
  "anya-e1008n:205": "Asobhagyaṁ jaro thīnaṁ,",
  "anya-e1008n:206": "Assānaṁ methunaṁ jaro.",
  "anya-e1008n:207": "42.",
  "anya-e1008n:208": " Atthi putto vase yassa,",
  "anya-e1008n:209": "Bhacco bhariyā tatheva ca;",
  "anya-e1008n:210": "Abhāve pyatisantoso,",
  "anya-e1008n:211": "Saggaṭṭho so mahītale.",
  "anya-e1008n:212": "43.",
  "anya-e1008n:213": " Duṭṭhā bhariyā saṭhaṁ mittaṁ,",
  "anya-e1008n:214": "Bhacco cuttaradāyako;",
  "anya-e1008n:215": "Sa sappeca gahe vāso,",
  "anya-e1008n:216": "Maccureva nasaṁsayo.",
  "anya-e1008n:217": "44.",
  "anya-e1008n:218": " Mātā yassa gehe natthi,",
  "anya-e1008n:219": "Bhariyācā piyavādinī;",
  "anya-e1008n:220": "Araññaṁ tena gantabbaṁ,",
  "anya-e1008n:221": "Yathā raññaṁ tathāgahaṁ.",
  "anya-e1008n:222": "45.",
  "anya-e1008n:223": " Iṇakattā pitā sattu,",
  "anya-e1008n:224": "Mātā ca byabhicārinī;",
  "anya-e1008n:225": "Bhariyā rūpavatī sattu,",
  "anya-e1008n:226": "Putto sattu apaṇḍito.",
  "anya-e1008n:227": "46.",
  "anya-e1008n:228": " Kokilānaṁ saro rūpaṁ,",
  "anya-e1008n:229": "Nārī rūpaṁ patibbatā;",
  "anya-e1008n:230": "Vijjā rūpaṁ kurūpānaṁ,",
  "anya-e1008n:231": "Khamā rūpaṁ tapassinaṁ.",
  "anya-e1008n:232": "47.",
  "anya-e1008n:233": " Avijjaṁ jīvanaṁ suññaṁ,",
  "anya-e1008n:234": "Disā suññā abandhavā;",
  "anya-e1008n:235": "Puttahīnaṁ gahaṁ suññaṁ,",
  "anya-e1008n:236": "Sabbasuññā daliddatā.",
  "anya-e1008n:237": "48.",
  "anya-e1008n:238": " Adātā vaṁsadosena,",
  "anya-e1008n:239": "Kammadosā daliddatā;",
  "anya-e1008n:240": "Ummādā mātudosena,",
  "anya-e1008n:241": "Pitudosena mūḷhatā.",
  "anya-e1008n:242": "49.",
  "anya-e1008n:243": " Guru aggi dvijādīnaṁ,",
  "anya-e1008n:244": "Vaṇṇānaṁ brāhmaṇo guru;",
  "anya-e1008n:245": "Pati reko gurutthīnaṁ,",
  "anya-e1008n:246": "Sabbassābhyāgato guru.",
  "anya-e1008n:247": "50.",
  "anya-e1008n:248": " Atidabbe hatā laṅkā,",
  "anya-e1008n:249": "Atimāne ca koravā;",
  "anya-e1008n:250": "Atidāne balībaddho,",
  "anya-e1008n:251": "Sabbamaccanta gahitaṁ.",
  "anya-e1008n:252": "51.",
  "anya-e1008n:253": " Vatthahīno tvalaṅkāro,",
  "anya-e1008n:254": "Ghatahīnañca bhojanaṁ;",
  "anya-e1008n:255": "Thanahīnā ca yānārī,",
  "anya-e1008n:256": "Vijjāhīnañca jīvanaṁ.",
  "anya-e1008n:257": "52.",
  "anya-e1008n:258": " Bhojjaṁ bhojanasatti ca,",
  "anya-e1008n:259": "Ratisatti varā thiyo;",
  "anya-e1008n:260": "Vibhavo dānasatti ca,",
  "anya-e1008n:261": "Nāppassa tapaso phalaṁ.",
  "anya-e1008n:262": "53.",
  "anya-e1008n:263": " Puttappayojanā dārā,",
  "anya-e1008n:264": "Putto piṇḍappayojano;",
  "anya-e1008n:265": "Hitappayojanaṁ mittaṁ,",
  "anya-e1008n:266": "Dhanaṁ sabbappayojanaṁ.",
  "anya-e1008n:267": "54.",
  "anya-e1008n:268": " Dullabhaṁ pākatikaṁ vākyaṁ,",
  "anya-e1008n:269": "Dullabho khemakaro suto;",
  "anya-e1008n:270": "Dullabhā sadisī jāyā,",
  "anya-e1008n:271": "Dullabho sajano piyo.",
  "anya-e1008n:272": "55.",
  "anya-e1008n:273": " Selesele namāṇikkaṁ,",
  "anya-e1008n:274": "Muttikaṁ na gajegaje;",
  "anya-e1008n:275": "Sādhavo na hi sabbatra,",
  "anya-e1008n:276": "Candanaṁ na vanevane.",
  "anya-e1008n:277": "56.",
  "anya-e1008n:278": " Asoco niddhano pañño,",
  "anya-e1008n:279": "Asoco paṇḍitabandhavo;",
  "anya-e1008n:280": "Asocā vidhavā nārī,",
  "anya-e1008n:281": "Puttanatta patiṭṭhitā.",
  "anya-e1008n:282": "57.",
  "anya-e1008n:283": " Avijjo puriso soco,",
  "anya-e1008n:284": "Socaṁ methuna mappajaṁ;",
  "anya-e1008n:285": "Nirāhārā pajā socā,",
  "anya-e1008n:286": "Socaṁ rajja marājakaṁ.",
  "anya-e1008n:287": "58.",
  "anya-e1008n:288": " Kulehi saha sampakkaṁ,",
  "anya-e1008n:289": "Paṇḍitehi ca mittataṁ;",
  "anya-e1008n:290": "Ñātībhi ca samaṁ melaṁ,",
  "anya-e1008n:291": "Kubbāno navinassati.",
  "anya-e1008n:292": "59.",
  "anya-e1008n:293": " Kaṭṭhā vutti parādhinā,",
  "anya-e1008n:294": "Kaṭṭho vāso nirāsayo;",
  "anya-e1008n:295": "Niddhano byavasāyo ca,",
  "anya-e1008n:296": "Sabbakaṭṭhā daliddatā.",
  "anya-e1008n:297": "60.",
  "anya-e1008n:298": " Takkarassa kuto dhammo,",
  "anya-e1008n:299": "Dujjanassa kuto khamā;",
  "anya-e1008n:300": "Vesiyā ca kuto sneho,",
  "anya-e1008n:301": "Kuto saccañca kāminaṁ.",
  "anya-e1008n:302": "61.",
  "anya-e1008n:303": " Pesitassa kuto mānaṁ,",
  "anya-e1008n:304": "Kopanassa kuto sukhaṁ;",
  "anya-e1008n:305": "Thīnaṁ kuto satittañca,",
  "anya-e1008n:306": "Kuto mettī khalassa ca.",
  "anya-e1008n:307": "62.",
  "anya-e1008n:308": " Dubbalassa balaṁ rājā,",
  "anya-e1008n:309": "Bālānaṁ rodanaṁ balaṁ;",
  "anya-e1008n:310": "Balaṁmūḷhassa monittaṁ,",
  "anya-e1008n:311": "Corānaṁ atathaṁ balaṁ.",
  "anya-e1008n:312": "63.",
  "anya-e1008n:313": " Yo dhuvāni pariccajja,",
  "anya-e1008n:314": "Adhuvaṁ parisevati;",
  "anya-e1008n:315": "Dhuvāni tassa nassanti,",
  "anya-e1008n:316": "Adhuvaṁ naṭṭhameva ca.",
  "anya-e1008n:317": "64.",
  "anya-e1008n:318": " Sukkaṁ maṁsaṁ thiyo vuddhā,",
  "anya-e1008n:319": "Bālakka taruṇaṁ dadhi;",
  "anya-e1008n:320": "Pabhāte methunaṁ niddā,",
  "anya-e1008n:321": "Sajju pāṇaharāni cha;",
  "anya-e1008n:322": "65.",
  "anya-e1008n:323": " Sajju maṁsaṁ navannañca,",
  "anya-e1008n:324": "Bālā thī khīrabhojanaṁ;",
  "anya-e1008n:325": "Ghatamuṇhodakañceva,",
  "anya-e1008n:326": "Sajju pāṇakarāni cha.",
  "anya-e1008n:327": "66.",
  "anya-e1008n:328": " Sīhādekaṁ bakādekaṁ,",
  "anya-e1008n:329": "Cha sunā tīṇi gadrabhā;",
  "anya-e1008n:330": "Vāyasā catu sikkhetha,",
  "anya-e1008n:331": "Cattāri kukkuṭādapi.",
  "anya-e1008n:332": "67.",
  "anya-e1008n:333": " Pabhūtamappakiccaṁ vā,",
  "anya-e1008n:334": "Yonaro kattumicchati;",
  "anya-e1008n:335": "Saṁyatanena kattabbaṁ,",
  "anya-e1008n:336": "Sīhādekaṁ pakittitaṁ.",
  "anya-e1008n:337": "68.",
  "anya-e1008n:338": " Sabbindriyāni saṁyama,",
  "anya-e1008n:339": "Bakova patito jano;",
  "anya-e1008n:340": "Kāladesopapannāni,",
  "anya-e1008n:341": "Sabbakiccāni sādhaye.",
  "anya-e1008n:342": "69.",
  "anya-e1008n:343": " Bahvāsī sāppasantuṭṭho,",
  "anya-e1008n:344": "Suniddo sīghacetano;",
  "anya-e1008n:345": "Pabhubhatto ca sūro ca,",
  "anya-e1008n:346": "Ñātabbā cha sunā guṇā.",
  "anya-e1008n:347": "70.",
  "anya-e1008n:348": " Avissāmaṁ vahe bhāraṁ,",
  "anya-e1008n:349": "Sītuṇhañca navindati;",
  "anya-e1008n:350": "Sa santoso tathā niccaṁ,",
  "anya-e1008n:351": "Tīṇi sikkhetha gadrabhā.",
  "anya-e1008n:352": "71.",
  "anya-e1008n:353": " Guḷhamethunadhammañca,",
  "anya-e1008n:354": "Kālekāle ca saṅgahaṁ;",
  "anya-e1008n:355": "Appamādamanālasyaṁ,",
  "anya-e1008n:356": "Catu sikkhetha vāyasā.",
  "anya-e1008n:357": "72.",
  "anya-e1008n:358": " Yuddhañca pātaruṭṭhānaṁ,",
  "anya-e1008n:359": "Bhojanaṁ saha bandhuhi;",
  "anya-e1008n:360": "Thiyaṁ āpadaggataṁ rakkhe,",
  "anya-e1008n:361": "Catu sikkhetha kukkuṭā.",
  "anya-e1008n:362": "73.",
  "anya-e1008n:363": " Kotibhāro samatthānaṁ,",
  "anya-e1008n:364": "Kiṁdūraṁ byavasāyinaṁ;",
  "anya-e1008n:365": "Ko videso savijjānaṁ,",
  "anya-e1008n:366": "Ko paro piyavādinaṁ.",
  "anya-e1008n:367": "74.",
  "anya-e1008n:368": " Bhayassa kathito pantho,",
  "anya-e1008n:369": "Indriyānamasaṁyamo;",
  "anya-e1008n:370": "Tajjayo sampadāmaggo,",
  "anya-e1008n:371": "Yeniṭṭhaṁ tena gamyate.",
  "anya-e1008n:372": "75.",
  "anya-e1008n:373": " Na ca vijjāsamo bandhu,",
  "anya-e1008n:374": "Na ca byādhisamo ripu;",
  "anya-e1008n:375": "Nacāpaccasamo sneho,",
  "anya-e1008n:376": "Na ca devā paraṁ balaṁ.",
  "anya-e1008n:377": "76.",
  "anya-e1008n:378": " Samuddāvaraṇā bhūmi,",
  "anya-e1008n:379": "Pākārāvaraṇaṁ gahaṁ;",
  "anya-e1008n:380": "Narindāvaraṇā desā,",
  "anya-e1008n:381": "Cārittāvaraṇā thiyo.",
  "anya-e1008n:382": "77.",
  "anya-e1008n:383": " Ghatakumbhasamā nārī,",
  "anya-e1008n:384": "Tattaṅgāra samo pumā;",
  "anya-e1008n:385": "Tasmā ghatañca aggiñca,",
  "anya-e1008n:386": "Nekatra thapaye budho.",
  "anya-e1008n:387": "78.",
  "anya-e1008n:388": " Āhāro dviguṇo thīnaṁ,",
  "anya-e1008n:389": "Buddhi tāsaṁ catugguṇo;",
  "anya-e1008n:390": "Chaguṇo byavasāyo ca,",
  "anya-e1008n:391": "Kāmocaṭṭhaguṇo mato.",
  "anya-e1008n:392": "79.",
  "anya-e1008n:393": " Jiṇṇamannaṁ pasaṁseyya,",
  "anya-e1008n:394": "Bhariyaṁ gatayobbanaṁ;",
  "anya-e1008n:395": "Raṇā paccāgataṁ sūraṁ,",
  "anya-e1008n:396": "Sassañca gehamāgataṁ.",
  "anya-e1008n:397": "80.",
  "anya-e1008n:398": " Asantuṭṭhā dvijā naṭṭhā,",
  "anya-e1008n:399": "Santuṭṭhāiva pāthivā;",
  "anya-e1008n:400": "Salajjā gaṇikā naṭṭhā,",
  "anya-e1008n:401": "Nillajjā ca kulitthiyo.",
  "anya-e1008n:402": "81.",
  "anya-e1008n:403": " Avaṁsapatito rājā,",
  "anya-e1008n:404": "Mūḷhaputto ca paṇḍito;",
  "anya-e1008n:405": "Adhanena dhanaṁ pāpya,",
  "anya-e1008n:406": "Tiṇaṁva maññate janaṁ.",
  "anya-e1008n:407": "82.",
  "anya-e1008n:408": " Brahmahāpi naro pujjo,",
  "anya-e1008n:409": "Yassatthi vipulaṁ dhanaṁ;",
  "anya-e1008n:410": "Sasino tulyavaṁsopi,",
  "anya-e1008n:411": "Niddhano paribhūyate.",
  "anya-e1008n:412": "83.",
  "anya-e1008n:413": " Potthakaṭṭhā tu yāvijjā,",
  "anya-e1008n:414": "Parahatthagataṁ dhanaṁ;",
  "anya-e1008n:415": "Kiccakāle samuppanne,",
  "anya-e1008n:416": "Na sāvijjā na taddhanaṁ.",
  "anya-e1008n:417": "84.",
  "anya-e1008n:418": " Pādapānaṁ bhayaṁ vātā,",
  "anya-e1008n:419": "Padmānaṁ sisirā bhayaṁ;",
  "anya-e1008n:420": "Pabbatānaṁ vajīramhā,",
  "anya-e1008n:421": "Sādhūnaṁ dujjanā bhayaṁ.",
  "anya-e1008n:422": "85.",
  "anya-e1008n:423": " Paññe niyujjamāne tu,",
  "anya-e1008n:424": "Santi rañño tayoguṇā;",
  "anya-e1008n:425": "Yaso sagganivāso ca,",
  "anya-e1008n:426": "Vipulo ca dhanāgamo.",
  "anya-e1008n:427": "86.",
  "anya-e1008n:428": " Mūḷhe niyujjamānetu,",
  "anya-e1008n:429": "Khattiyassāguṇā tayo;",
  "anya-e1008n:430": "Ayaso catthanāso ca,",
  "anya-e1008n:431": "Narake gamanaṁ tathā.",
  "anya-e1008n:432": "87.",
  "anya-e1008n:433": " Bahūmūḷhasaṅghātehi,",
  "anya-e1008n:434": "Aññoññapasuvuttibhi;",
  "anya-e1008n:435": "Pacchādyante guṇā sabbe,",
  "anya-e1008n:436": "Meghehiva divākaro.",
  "anya-e1008n:437": "88.",
  "anya-e1008n:438": " Yassa khettaṁ nadītīre,",
  "anya-e1008n:439": "Bhariyāpi parappiyā;",
  "anya-e1008n:440": "Puttassa vinayo natthi,",
  "anya-e1008n:441": "Maccureva nasaṁsayo.",
  "anya-e1008n:442": "89.",
  "anya-e1008n:443": " Asambhābyaṁ navattabbaṁ,",
  "anya-e1008n:444": "Paccakkhamapi dissate;",
  "anya-e1008n:445": "Silā tarati pānīyaṁ,",
  "anya-e1008n:446": "Gītaṁ gāyati vānaro.",
  "anya-e1008n:447": "90.",
  "anya-e1008n:448": " Subhikkhaṁ kasake niccaṁ,",
  "anya-e1008n:449": "Niccaṁ sukha marogike;",
  "anya-e1008n:450": "Bhariyā bhattu piyā yassa,",
  "anya-e1008n:451": "Tassa niccossavaṁ gahaṁ.",
  "anya-e1008n:452": "91.",
  "anya-e1008n:453": " Helassa kammanāsāya,",
  "anya-e1008n:454": "Buddhināsāya niddhanaṁ;",
  "anya-e1008n:455": "Yācanā mānanāsāya,",
  "anya-e1008n:456": "Kulanāsāya bhojanaṁ.",
  "anya-e1008n:457": "92.",
  "anya-e1008n:458": " Sevitabbo mahāvakkho,",
  "anya-e1008n:459": "Phalacchāyā samanvito;",
  "anya-e1008n:460": "Yadi devā phalaṁ natthi,",
  "anya-e1008n:461": "Chāyā kena nivāraye.",
  "anya-e1008n:462": "93.",
  "anya-e1008n:463": " Paṭhame najjitā vijjā,",
  "anya-e1008n:464": "Dutīye najjitaṁ dhanaṁ;",
  "anya-e1008n:465": "Tatīye najjitaṁ puññaṁ,",
  "anya-e1008n:466": "Catutthe kiṁkarissati.",
  "anya-e1008n:467": "94.",
  "anya-e1008n:468": " Nadīkūleca ye vakkhā,",
  "anya-e1008n:469": "Parahatthagataṁ dhanaṁ;",
  "anya-e1008n:470": "Kiccaṁ thīgocaraṁ yassa,",
  "anya-e1008n:471": "Sabbaṁ taṁ viphalaṁ bhave.",
  "anya-e1008n:472": "95.",
  "anya-e1008n:473": " Kudesamāsajja kutotthasañcayo,",
  "anya-e1008n:474": "Kuputtamāsajja kuto jalañjalī;",
  "anya-e1008n:475": "Kugehiniṁ pāpya gahe kuto sukhaṁ,",
  "anya-e1008n:476": "Kusissamajjhāpayato kuto yaso.",
  "anya-e1008n:477": "96.",
  "anya-e1008n:478": " Kūpodakaṁ vaṭacchāyā,",
  "anya-e1008n:479": "Sāmā thīciṭṭhakālayaṁ;",
  "anya-e1008n:480": "Sītakāle bhave uṇhaṁ,",
  "anya-e1008n:481": "Gimhakāle ca sītalaṁ.",
  "anya-e1008n:482": "97.",
  "anya-e1008n:483": " Visaṁ caṅkamanaṁ rattiṁ,",
  "anya-e1008n:484": "Visaṁ raññonukulatā;",
  "anya-e1008n:485": "Visaṁ thīpi aññāsattā,",
  "anya-e1008n:486": "Visaṁ byādhi avekkhito.",
  "anya-e1008n:487": "98.",
  "anya-e1008n:488": " Duradhītā visaṁ vijjā,",
  "anya-e1008n:489": "Ajiṇṇe bhojanaṁ visaṁ;",
  "anya-e1008n:490": "Visaṁ goṭṭhī daliddassa,",
  "anya-e1008n:491": "Vuddhassa taruṇī visaṁ.",
  "anya-e1008n:492": "99.",
  "anya-e1008n:493": " Padose nihato pantho,",
  "anya-e1008n:494": "Patitā nihatā thiyo;",
  "anya-e1008n:495": "Appabījaṁ hataṁ khettaṁ,",
  "anya-e1008n:496": "Bhaccadosā hato pabhū.",
  "anya-e1008n:497": "100.",
  "anya-e1008n:498": " Hatamasottiyaṁ saddhaṁ,",
  "anya-e1008n:499": "Hato yañño tvadakkhiṇo;",
  "anya-e1008n:500": "Hatā rūpavatī vañjhā,",
  "anya-e1008n:501": "Hataṁ senamanāyakaṁ.",
  "anya-e1008n:502": "101.",
  "anya-e1008n:503": " Vedavedaṅga tattañño,",
  "anya-e1008n:504": "Japahomaparāyano;",
  "anya-e1008n:505": "Āsīvādavacoyutto,",
  "anya-e1008n:506": "Esa rājapurohito.",
  "anya-e1008n:507": "102.",
  "anya-e1008n:508": " Kulasīlaguṇopeto,",
  "anya-e1008n:509": "Sabbadhammaparāyano;",
  "anya-e1008n:510": "Pavīṇo pesanādyakkho,",
  "anya-e1008n:511": "Dhammādyakkho vidhīyate.",
  "anya-e1008n:512": "103.",
  "anya-e1008n:513": " Ayubbedakatābhyāso,",
  "anya-e1008n:514": "Sabbesaṁ piyadassano;",
  "anya-e1008n:515": "Ariyasīlaguṇopeto,",
  "anya-e1008n:516": "Esa vajjo vidhīyate.",
  "anya-e1008n:517": "104.",
  "anya-e1008n:518": " Sakiṁdutta gahitattho,",
  "anya-e1008n:519": "Lahuhattho jitakkharo;",
  "anya-e1008n:520": "Sabbasattha samālokī,",
  "anya-e1008n:521": "Pakaṭṭho nāma lekhako.",
  "anya-e1008n:522": "105.",
  "anya-e1008n:523": " Samattanītisattañño,",
  "anya-e1008n:524": "Vāhane pūjitassamo;",
  "anya-e1008n:525": "Sūravīraguṇopeto,",
  "anya-e1008n:526": "Senādhyakkho vidhīyate.",
  "anya-e1008n:527": "106.",
  "anya-e1008n:528": " Sucī vākyapaṭuppañño,",
  "anya-e1008n:529": "Paracittopalakkhako;",
  "anya-e1008n:530": "Dhīro yathāttha vādī ca,",
  "anya-e1008n:531": "Esa dūto vidhīyate.",
  "anya-e1008n:532": "107.",
  "anya-e1008n:533": " Puttanatta guṇopeto,",
  "anya-e1008n:534": "Satthañño piṭṭhapācako;",
  "anya-e1008n:535": "Sūro ca kathinoceva,",
  "anya-e1008n:536": "Sūpakāro sa uccate.",
  "anya-e1008n:537": "108.",
  "anya-e1008n:538": " Iṅgitā kāratattañño,",
  "anya-e1008n:539": "Balavā piyadassano;",
  "anya-e1008n:540": "Appamādī sadā dakkho,",
  "anya-e1008n:541": "Patihāro sa uccate.",
  "anya-e1008n:542": "109.",
  "anya-e1008n:543": " Yassa natthi sayaṁ paññā,",
  "anya-e1008n:544": "Satthaṁ tassa karoti kiṁ;",
  "anya-e1008n:545": "Locanehi vihīnassa,",
  "anya-e1008n:546": "Dappaṇo kiṁkarissati.",
  "anya-e1008n:547": "110.",
  "anya-e1008n:548": " Kiṁkarissanti vattāro,",
  "anya-e1008n:549": "Sotaṁ yattha navijjate;",
  "anya-e1008n:550": "Naggakapaṇake dese,",
  "anya-e1008n:551": "Rajaṇo kiṁkarissati."
}