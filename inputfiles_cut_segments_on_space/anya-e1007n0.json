{
  "anya-e1007n0:0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1007n0:1": "Sūrassatīnīti",
  "anya-e1007n0:2": "Paṭhamo bhāgo",
  "anya-e1007n0:3": "Paṇāmagāthā",
  "anya-e1007n0:4": "1.",
  "anya-e1007n0:5": "Mukhamhā bhagavantassa,",
  "anya-e1007n0:6": "Sugandhakamalā subhā;",
  "anya-e1007n0:7": "Sañjātaṃ uttamaṃ vāṇiṃ,",
  "anya-e1007n0:8": "Vandāmi vittamānasā.",
  "anya-e1007n0:9": "2.",
  "anya-e1007n0:10": "Samajjhiṭṭho racissāmi,",
  "anya-e1007n0:11": "Dakkhena rājamantinā;",
  "anya-e1007n0:12": "Nāmena santena,",
  "anya-e1007n0:13": "Nītiṃ lokahitāvahaṃ;",
  "anya-e1007n0:14": "Sālaṅkāraṃ sopadesaṃ,",
  "anya-e1007n0:15": "Nāmenāhaṃ svarassatiṃ.",
  "anya-e1007n0:16": "3.",
  "anya-e1007n0:17": "Pekkhantvimaṃ nītiṃ santā,",
  "anya-e1007n0:18": "Mañjūsarukkhasannibhaṃ;",
  "anya-e1007n0:19": "Nānopadesasaṃpuṇṇaṃ,",
  "anya-e1007n0:20": "Sadatthakusalā sadā.",
  "anya-e1007n0:21": "Sūrassatīnīti",
  "anya-e1007n0:22": "1.",
  "anya-e1007n0:23": "Kataññutā ca saccañca,",
  "anya-e1007n0:24": "Lokasārā hi te duve;",
  "anya-e1007n0:25": "Lokāpi tehi tiṭṭhanti,",
  "anya-e1007n0:26": "Raṭṭhaṃ akaṃsu issaraṃ.",
  "anya-e1007n0:27": "2.",
  "anya-e1007n0:28": "Kākāca dujjanā loke,",
  "anya-e1007n0:29": "Malībhūtāva sabbadā;",
  "anya-e1007n0:30": "Iṭṭhaṃ guṇaṃ nāsayanti,",
  "anya-e1007n0:31": "Te ve lokassa verino.",
  "anya-e1007n0:32": "Migānaṃ siṅgālo anto,",
  "anya-e1007n0:33": "Pakkhīnaṃ pana vāyaso.",
  "anya-e1007n0:34": "Akāraṇaverī honti,",
  "anya-e1007n0:35": "Macchānaṃ dhīvarā yathā;",
  "anya-e1007n0:36": "Guṇīnaṃ sajjanānañca,",
  "anya-e1007n0:37": "Dujjanā niccaverino.",
  "anya-e1007n0:38": "3.",
  "anya-e1007n0:39": "Siriṃ bhonto suposetha,",
  "anya-e1007n0:40": "Siri mūlā hi sampadā;",
  "anya-e1007n0:41": "Siriya idha jotanti,",
  "anya-e1007n0:42": "Sirī sā sabbasiddhikā.",
  "anya-e1007n0:43": "4.",
  "anya-e1007n0:44": "Musā tamokarā loke,",
  "anya-e1007n0:45": "Saccaṃ mīditikārakaṃ;",
  "anya-e1007n0:46": "Musātamena dukkhanti,",
  "anya-e1007n0:47": "Saccābhāya sukhantive.",
  "anya-e1007n0:48": "Suvijānaṃ siṅgālānaṃ,",
  "anya-e1007n0:49": "Sakuṇānañca vassitaṃ;",
  "anya-e1007n0:50": "Manussavassitaṃ rāja,",
  "anya-e1007n0:51": "Dubbijānataraṃ tato.",
  "anya-e1007n0:52": "5.",
  "anya-e1007n0:53": "Kappaggisadisā issā,",
  "anya-e1007n0:54": "Jhāpeti sabbasampadaṃ;",
  "anya-e1007n0:55": "Muditā kappameghova,",
  "anya-e1007n0:56": "Ropeti sabbasampadaṃ.",
  "anya-e1007n0:57": "6.",
  "anya-e1007n0:58": "Yathā asanthirā thambhā,",
  "anya-e1007n0:59": "Thusarāsimhi ussitā;",
  "anya-e1007n0:60": "Tatheva kapicittānaṃ,",
  "anya-e1007n0:61": "Kammantā cañcalaṅgatā.",
  "anya-e1007n0:62": "Haliddirāgaṃ kapicittaṃ;",
  "anya-e1007n0:63": "Purisaṃ rāgavirāginaṃ,",
  "anya-e1007n0:64": "Edisaṃ tāta māsevi;",
  "anya-e1007n0:65": "Nimmanussampi ce siyā.",
  "anya-e1007n0:66": "7. [Ka]",
  "anya-e1007n0:67": "Pupphacumbi cittapattī,",
  "anya-e1007n0:68": "Sakavaṇṇena majjito;",
  "anya-e1007n0:69": "Vikkamī pupphato pupphaṃ,",
  "anya-e1007n0:70": "Nassāgāraṃ na saṅgamo.",
  "anya-e1007n0:71": "[Kha]",
  "anya-e1007n0:72": "Kippilikā dubbaṇṇāpi,",
  "anya-e1007n0:73": "Samaggāca parakkamā;",
  "anya-e1007n0:74": "Mā hotha pupphacumbīva,",
  "anya-e1007n0:75": "Hotha vo pacikā yathā.",
  "anya-e1007n0:76": "1.",
  "anya-e1007n0:77": "Ye maranti kīḷantā te,",
  "anya-e1007n0:78": "Svānā aññoññamoditā;",
  "anya-e1007n0:79": "Disvāna chaṭṭitaṃ bhattaṃ,",
  "anya-e1007n0:80": "Sīghaverī vihiṃsare.",
  "anya-e1007n0:81": "2.",
  "anya-e1007n0:82": "Tathekepi janā dāni,",
  "anya-e1007n0:83": "Samaggā ññoññamoditā;",
  "anya-e1007n0:84": "Dhanahetu vihiṃsanti,",
  "anya-e1007n0:85": "Dhīratthu sīghaverikā.",
  "anya-e1007n0:86": "9.",
  "anya-e1007n0:87": "Vasante hemante gimhe;",
  "anya-e1007n0:88": "Neva tālā visesino;",
  "anya-e1007n0:89": "Thiracittā janā santā;",
  "anya-e1007n0:90": "Sukhadukkhesu niccalā.",
  "anya-e1007n0:91": "10.",
  "anya-e1007n0:92": "Yathā pavaṭṭamānamhi,",
  "anya-e1007n0:93": "Suṭṭhu tiṭṭhati geṇṭhuke;",
  "anya-e1007n0:94": "Appavaṭṭe bhūmyaṃ seti,",
  "anya-e1007n0:95": "Tatheva geṇṭhuko jano.",
  "anya-e1007n0:96": "11.",
  "anya-e1007n0:97": "Agghāpetuṃ nasakkonti,",
  "anya-e1007n0:98": "Kālañhi kālikā janā;",
  "anya-e1007n0:99": "Vajirādiñca sakkonti,",
  "anya-e1007n0:100": "Tena kālo anagghiko.",
  "anya-e1007n0:101": "12. [Ka]",
  "anya-e1007n0:102": "Asano hi dīghaddhāno,",
  "anya-e1007n0:103": "Sārasāro sugandhiko;",
  "anya-e1007n0:104": "Niggandho tveva nissāro,",
  "anya-e1007n0:105": "Dīghaddhānopi simbalī.",
  "anya-e1007n0:106": "[Kha]",
  "anya-e1007n0:107": "Tatheveke janā loke,",
  "anya-e1007n0:108": "Dīghaddhānā susārakā;",
  "anya-e1007n0:109": "Nissārā keci pheggūva,",
  "anya-e1007n0:110": "Dīghaddhānāpi goyathā.",
  "anya-e1007n0:111": "13.",
  "anya-e1007n0:112": "Upakāro cāpakāro,",
  "anya-e1007n0:113": "Yasmiṃ gacchati naṭṭhataṃ;",
  "anya-e1007n0:114": "Pāsāṇahadayassassa,",
  "anya-e1007n0:115": "Jīvatītyā bhidhāmudhā.",
  "anya-e1007n0:116": "Pasādo nipphalo yassa,",
  "anya-e1007n0:117": "Kopocāpi niratthako;",
  "anya-e1007n0:118": "Na taṃ saṅgantu miccheyya,",
  "anya-e1007n0:119": "Thīpumāva napuṃsakaṃ.",
  "anya-e1007n0:120": "14.",
  "anya-e1007n0:121": "Upacāro hi kātabbo,",
  "anya-e1007n0:122": "Na yāva sohadaṃ bhave;",
  "anya-e1007n0:123": "Upacāro sumittamhi,",
  "anya-e1007n0:124": "Māyā ca hoti koṭilaṃ.",
  "anya-e1007n0:125": "15.",
  "anya-e1007n0:126": "Kamena aggato ucchu,",
  "anya-e1007n0:127": "Raso sādutaro yathā;",
  "anya-e1007n0:128": "Tatheva sumitto loke,",
  "anya-e1007n0:129": "Dummitto pana nediso.",
  "anya-e1007n0:130": "16.",
  "anya-e1007n0:131": "Sokārāti parittāṇaṃ,",
  "anya-e1007n0:132": "Vissāsapītibhājanaṃ;",
  "anya-e1007n0:133": "Ratanābhiratanaṃ icche,",
  "anya-e1007n0:134": "Sumittaṃ akkharattayaṃ.",
  "anya-e1007n0:135": "17.",
  "anya-e1007n0:136": "Dampatīnaṃ sumittānaṃ,",
  "anya-e1007n0:137": "Mukhaṃ aññoññadappaṇaṃ,",
  "anya-e1007n0:138": "Sukhe sukhaṃ dukkhe dukkhaṃ,",
  "anya-e1007n0:139": "Paṭicchāyeva dappaṇe.",
  "anya-e1007n0:140": "Kathaṃ nu tāsaṃ hadayaṃ,",
  "anya-e1007n0:141": "Sukharāvata itthiyo;",
  "anya-e1007n0:142": "Yāsāmike dukkhitamhi,",
  "anya-e1007n0:143": "Sukhamicchanti attano.",
  "anya-e1007n0:144": "18.",
  "anya-e1007n0:145": "Nivātañca pure katvā,",
  "anya-e1007n0:146": "Mānaṃ katvāna pacchato;",
  "anya-e1007n0:147": "Sakatthaṃ dhāraye dhīro,",
  "anya-e1007n0:148": "Atthabhañjo hi muḷhatā.",
  "anya-e1007n0:149": "19.",
  "anya-e1007n0:150": "Purecāraṃ satiṃ katvā,",
  "anya-e1007n0:151": "Saddhaṃ kareyya pacchato;",
  "anya-e1007n0:152": "Turaṃ na saddahe dhīro,",
  "anya-e1007n0:153": "Sīghasaddho hi mandako.",
  "anya-e1007n0:154": "20.",
  "anya-e1007n0:155": "Vane 0.0099 bahūni kaṭṭhāni;",
  "anya-e1007n0:156": "Dullabhaṃ rattacandanaṃ;",
  "anya-e1007n0:157": "Tathā janā bahū loke;",
  "anya-e1007n0:158": "Pumā jañño sudullabho.",
  "anya-e1007n0:159": "21.",
  "anya-e1007n0:160": "Tiṇakaṭṭhapalāsehi;",
  "anya-e1007n0:161": "Sukkhehi dayhate vanaṃ;",
  "anya-e1007n0:162": "Etādīhi asārehi;",
  "anya-e1007n0:163": "Loko janehi dayhate.",
  "anya-e1007n0:164": "22.",
  "anya-e1007n0:165": "Antovasse timāsamhi,",
  "anya-e1007n0:166": "Puññakammena moditā;",
  "anya-e1007n0:167": "Sukhaṃ vasiṃsu porāṇā,",
  "anya-e1007n0:168": "Buddhasāsanamāmakā.",
  "anya-e1007n0:169": "23.",
  "anya-e1007n0:170": "Migamadena ekena,",
  "anya-e1007n0:171": "Taṃ vanaṃ surabhigandhikaṃ;",
  "anya-e1007n0:172": "Tathā janena taṃ raṭṭhaṃ,",
  "anya-e1007n0:173": "Guṇinā hi sirīmatā.",
  "anya-e1007n0:174": "Sarīraṃ khaṇaviddhaṃsī,",
  "anya-e1007n0:175": "Kappantaṭṭhāyino guṇā.",
  "anya-e1007n0:176": "24.",
  "anya-e1007n0:177": "Kharānaṃ sīhabyagghānaṃ,",
  "anya-e1007n0:178": "Saṅgamo no hisabbadā;",
  "anya-e1007n0:179": "Tatheva byagghacittānaṃ,",
  "anya-e1007n0:180": "Sajātikā khayonatā.",
  "anya-e1007n0:181": "25.",
  "anya-e1007n0:182": "Saka sādhupi no sādhū,",
  "anya-e1007n0:183": "Yo ceñña duṭṭhakārako;",
  "anya-e1007n0:184": "Bahūnaṃ sādhū pāyena,",
  "anya-e1007n0:185": "Sa ve sādhūti vuccate.",
  "anya-e1007n0:186": "26.",
  "anya-e1007n0:187": "Bahūdake samuddepi,",
  "anya-e1007n0:188": "Jalaṃ nattheva pātave;",
  "anya-e1007n0:189": "Khuddake khatakūpamhi,",
  "anya-e1007n0:190": "Sāduṃ atthi bahuṃ dakaṃ.",
  "anya-e1007n0:191": "27.",
  "anya-e1007n0:192": "Mā sīghaṃ vivareyyātha,",
  "anya-e1007n0:193": "Nindituñca pasaṃsituṃ;",
  "anya-e1007n0:194": "Mukhañhi vo kathādvāraṃ,",
  "anya-e1007n0:195": "Nirundheyyātha sabbadā.",
  "anya-e1007n0:196": "28.",
  "anya-e1007n0:197": "Mā sīghaṃ vivareyyātha,",
  "anya-e1007n0:198": "Cakkhuṃ vo dassituṃ piyaṃ;",
  "anya-e1007n0:199": "Saṇikañhi piyalābhaṃ,",
  "anya-e1007n0:200": "Dhanalābhaṃ turaṃ kare.",
  "anya-e1007n0:201": "29.",
  "anya-e1007n0:202": "Anārambho hi kammānaṃ,",
  "anya-e1007n0:203": "Paṭhamaṃ buddhilakkhaṇaṃ;",
  "anya-e1007n0:204": "Niṭṭhaṅgataṃ āraddhassa,",
  "anya-e1007n0:205": "Dutiyaṃ buddhilakkhaṇaṃ.",
  "anya-e1007n0:206": "Asamekkhitakammantaṃ,",
  "anya-e1007n0:207": "Turitābhinipātinaṃ;",
  "anya-e1007n0:208": "Sāni kammāni tappenti,",
  "anya-e1007n0:209": "Uṇhaṃ vajjhohaṭaṃ mukhe.",
  "anya-e1007n0:210": "30.",
  "anya-e1007n0:211": "Aphalāni durantāni,",
  "anya-e1007n0:212": "Janatā ninditāni ca;",
  "anya-e1007n0:213": "Asakyāni ca kammāni,",
  "anya-e1007n0:214": "Nārabhetha vicakkhaṇo.",
  "anya-e1007n0:215": "31.",
  "anya-e1007n0:216": "Ativirodhabhītānaṃ,",
  "anya-e1007n0:217": "Saṅkitānaṃ pade pade;",
  "anya-e1007n0:218": "Parappavādatāsānaṃ,",
  "anya-e1007n0:219": "Dūrato yanti sampadā.",
  "anya-e1007n0:220": "Saddamattaṃ na bhetabbaṃ,",
  "anya-e1007n0:221": "Loko saddassa gocaro;",
  "anya-e1007n0:222": "Yo ca saddaparittāso,",
  "anya-e1007n0:223": "Vane bhantamigo hi so.",
  "anya-e1007n0:224": "32.",
  "anya-e1007n0:225": "Dvinnaṃ taṇḍulathūsānaṃ,",
  "anya-e1007n0:226": "Viseso suṭṭhu khāyati;",
  "anya-e1007n0:227": "Randhitopi siniddho no,",
  "anya-e1007n0:228": "Thuso virasaphāruso;",
  "anya-e1007n0:229": "Taṇḍulaṃ siniddhaṃ rasaṃ,",
  "anya-e1007n0:230": "Evaṃ lokepi ñāyate.",
  "anya-e1007n0:231": "33.",
  "anya-e1007n0:232": "Eraṇḍaṃ nissitā valli,",
  "anya-e1007n0:233": "Ruhate kiṃ yathābalaṃ;",
  "anya-e1007n0:234": "Mahāsālaṃ sunissāya,",
  "anya-e1007n0:235": "Ruhate brahataṃ gatā.",
  "anya-e1007n0:236": "34.",
  "anya-e1007n0:237": "Mettā hi sīmasambhedā,",
  "anya-e1007n0:238": "Pakkhapāta vighātikā;",
  "anya-e1007n0:239": "Pakkhapātena dukkhanti,",
  "anya-e1007n0:240": "Nippakkho vasate sukhaṃ.",
  "anya-e1007n0:241": "35.",
  "anya-e1007n0:242": "Narā paññā ca laṅkārā,",
  "anya-e1007n0:243": "Yathāṭhāne niyujjare;",
  "anya-e1007n0:244": "No hi cūḷāmaṇi pāde,",
  "anya-e1007n0:245": "Pādukā ca siropari.",
  "anya-e1007n0:246": "Ukkuṭṭhe sūra micchanti;",
  "anya-e1007n0:247": "Mantīsu akutūhalaṃ;",
  "anya-e1007n0:248": "Viyañca annapānamhi;",
  "anya-e1007n0:249": "Atthe jāte ca paṇḍitaṃ.",
  "anya-e1007n0:250": "36.",
  "anya-e1007n0:251": "Pamādo hi tamo loke,",
  "anya-e1007n0:252": "Kālo coro bhayānako;",
  "anya-e1007n0:253": "Kāyagehaṃ bahuchiddaṃ,",
  "anya-e1007n0:254": "Kālacorassa coritaṃ.",
  "anya-e1007n0:255": "37.",
  "anya-e1007n0:256": "Cañcalo kāladāso hi,",
  "anya-e1007n0:257": "Dhītimā kālaissaro;",
  "anya-e1007n0:258": "Kālissaro raṭṭhissaraṃ,",
  "anya-e1007n0:259": "Ativattati sabbaso.",
  "anya-e1007n0:260": "38.",
  "anya-e1007n0:261": "Viluppanti 8 dhanaṃ eke,",
  "anya-e1007n0:262": "Kālameke anekkhakā;",
  "anya-e1007n0:263": "Tesu kālavilopāva,",
  "anya-e1007n0:264": "Bhayānakā tikakkhaḷā.",
  "anya-e1007n0:265": "39.",
  "anya-e1007n0:266": "Raññā raṭṭhahitaṃ kattā,",
  "anya-e1007n0:267": "Rañño hitaṃ janehi ve;",
  "anya-e1007n0:268": "Desso attahitaṃ dassī,",
  "anya-e1007n0:269": "Gārayho kinnu kārako.",
  "anya-e1007n0:270": "Attadatthaṃ paratthena;",
  "anya-e1007n0:271": "Bahunāpi na hāpaye.",
  "anya-e1007n0:272": "40.",
  "anya-e1007n0:273": "Yassa upakāro dinno,",
  "anya-e1007n0:274": "Upakāraṃ dade puna;",
  "anya-e1007n0:275": "Tato pakāraṃ niccheyya,",
  "anya-e1007n0:276": "Kataññū dullabho idha.",
  "anya-e1007n0:277": "Saccaṃ kireva māhaṃsu,",
  "anya-e1007n0:278": "Narā ekacciyā idha;",
  "anya-e1007n0:279": "Kaṭṭhaṃ niplavitaṃ seyyo,",
  "anya-e1007n0:280": "Natveve kacciyo naro.",
  "anya-e1007n0:281": "41.",
  "anya-e1007n0:282": "Kāruko sakapaññāya,",
  "anya-e1007n0:283": "Mahagghaṃ dārukaṃ kare;",
  "anya-e1007n0:284": "Tathā janopi attānaṃ,",
  "anya-e1007n0:285": "Mahaggho lokamānito.",
  "anya-e1007n0:286": "42.",
  "anya-e1007n0:287": "Appaggho hi ayo hemaṃ,",
  "anya-e1007n0:288": "Mahagghaṃ chindate yathā;",
  "anya-e1007n0:289": "Nigguṇo saguṇaṃ loke,",
  "anya-e1007n0:290": "Alakkhīca siriṃ tathā.",
  "anya-e1007n0:291": "Sūrassatīnīti",
  "anya-e1007n0:292": "Dutiyo bhāgo",
  "anya-e1007n0:293": "1.",
  "anya-e1007n0:294": "Dhanassa dubbidhaṃ kiccaṃ,",
  "anya-e1007n0:295": "Pāpeti uṇṇataṃ dhaniṃ;",
  "anya-e1007n0:296": "Adhaniṃ oṇataṃ loke,",
  "anya-e1007n0:297": "Sañcine tena taṃ dhanaṃ.",
  "anya-e1007n0:298": "2.",
  "anya-e1007n0:299": "Vaṭṭate satataṃ sīghaṃ,",
  "anya-e1007n0:300": "Kālacakkaṃ avāritaṃ;",
  "anya-e1007n0:301": "Tena ghaṭī dinaṃ māso,",
  "anya-e1007n0:302": "Vasso bhavatya cīrato.",
  "anya-e1007n0:303": "3.",
  "anya-e1007n0:304": "Sattunā na hi sandheyya,",
  "anya-e1007n0:305": "Ekadā so bhayaṃ karo;",
  "anya-e1007n0:306": "Sutattamapi pānīyaṃ,",
  "anya-e1007n0:307": "Samayate nu pāvakaṃ.",
  "anya-e1007n0:308": "4.",
  "anya-e1007n0:309": "Vijahaṃ pakatiṃ yo hi,",
  "anya-e1007n0:310": "Vikatiṃ puna gacchati;",
  "anya-e1007n0:311": "Sabhāvena ākārena,",
  "anya-e1007n0:312": "Vippallāsaṃ sa gacchati;",
  "anya-e1007n0:313": "Saṃsumāra gatā godhā,",
  "anya-e1007n0:314": "Yathā thī pumavesikā.",
  "anya-e1007n0:315": "5.",
  "anya-e1007n0:316": "Pakkhaṃ laddhāna uḍḍenti,",
  "anya-e1007n0:317": "Upacikā hi vammikā;",
  "anya-e1007n0:318": "Nikkhantā maraṇaṃ yanti,",
  "anya-e1007n0:319": "Uppatā nippataṃ gatā.",
  "anya-e1007n0:320": "6.",
  "anya-e1007n0:321": "Sabbaṃpiyassa dajjeyya,",
  "anya-e1007n0:322": "Nissesaṃ piyamānasaṃ;",
  "anya-e1007n0:323": "Saddhācittaṃ tu no viññū,",
  "anya-e1007n0:324": "Saddhāyiko pakkhalito.",
  "anya-e1007n0:325": "7.",
  "anya-e1007n0:326": "Sakkoti laṅghituṃ byāmaṃ,",
  "anya-e1007n0:327": "Mahussāhena yo hi so;",
  "anya-e1007n0:328": "Tadaḍḍhaṃ anussāhena,",
  "anya-e1007n0:329": "Nossāho tesu thomito;",
  "anya-e1007n0:330": "Mahussāho dukkho loke,",
  "anya-e1007n0:331": "Anussāho sadā sukho.",
  "anya-e1007n0:332": "8.",
  "anya-e1007n0:333": "Dukkhamaṃ akkhamanto yo,",
  "anya-e1007n0:334": "Piṭṭhikārīca dukkaraṃ,",
  "anya-e1007n0:335": "Kadā labheyya so loke,",
  "anya-e1007n0:336": "Sukhamaṃ sukaraṃ mudhā;",
  "anya-e1007n0:337": "Paccakkhañhi so kareyya,",
  "anya-e1007n0:338": "Dukkhamañcāpi dukkaraṃ.",
  "anya-e1007n0:339": "9.",
  "anya-e1007n0:340": "Anaggho manusso loke,",
  "anya-e1007n0:341": "Tosanāposanādinā;",
  "anya-e1007n0:342": "Tena so mahagghaṃ kammaṃ,",
  "anya-e1007n0:343": "Kare lokahitāyutaṃ.",
  "anya-e1007n0:344": "10.",
  "anya-e1007n0:345": "Vātena nappabhijjanti,",
  "anya-e1007n0:346": "Ninnā veḷū kasā naḷā;",
  "anya-e1007n0:347": "Yathāvātaṃ nagacchanti,",
  "anya-e1007n0:348": "Tathā care jane kadā.",
  "anya-e1007n0:349": "11.",
  "anya-e1007n0:350": "Purato ca pacchato ce,",
  "anya-e1007n0:351": "Nissayo natthi passato;",
  "anya-e1007n0:352": "Adhikaṃ vīriyaṃ hoti,",
  "anya-e1007n0:353": "Attanātho tadā bhave.",
  "anya-e1007n0:354": "12.",
  "anya-e1007n0:355": "Khaṇaṃ ākhubilaṃ sīho,",
  "anya-e1007n0:356": "Pāsāṇasakalākulaṃ;",
  "anya-e1007n0:357": "Pappoti nakhabhaṅgaṃvā,",
  "anya-e1007n0:358": "Phalaṃvā mūsiko bhave.",
  "anya-e1007n0:359": "13.",
  "anya-e1007n0:360": "Maggamuḷhā janā andhā,",
  "anya-e1007n0:361": "Amaggā maggasaññino;",
  "anya-e1007n0:362": "Tatheva duppaññā muḷhā,",
  "anya-e1007n0:363": "Tathatthaṃ nāva bujjhare.",
  "anya-e1007n0:364": "14.",
  "anya-e1007n0:365": "Piyarūpaṃ vīrarūpaṃ,",
  "anya-e1007n0:366": "Duvidhā rūpasampadā,",
  "anya-e1007n0:367": "Nāriṃ icche piyarūpiṃ,",
  "anya-e1007n0:368": "Purisaṃ vīrarūpakaṃ.",
  "anya-e1007n0:369": "15.",
  "anya-e1007n0:370": "Pajjalanti hi khajjotā,",
  "anya-e1007n0:371": "Pakkhacālanakammunā;",
  "anya-e1007n0:372": "Kusitā supitā nete,",
  "anya-e1007n0:373": "Tathā janāpi kammikā.",
  "anya-e1007n0:374": "16.",
  "anya-e1007n0:375": "Kummasaṅkocamopamya,",
  "anya-e1007n0:376": "Niggahamapi saṃkhame;",
  "anya-e1007n0:377": "Pattakāle tu nītiñño,",
  "anya-e1007n0:378": "Kaṇhasappova uṭṭhahe.",
  "anya-e1007n0:379": "17.",
  "anya-e1007n0:380": "Bhakkhasesaṃ nakhādanti,",
  "anya-e1007n0:381": "Sīhā unnatacetasā;",
  "anya-e1007n0:382": "Paraṃpi napaṇāmenti,",
  "anya-e1007n0:383": "Vuddhikāmā tathā care.",
  "anya-e1007n0:384": "18.",
  "anya-e1007n0:385": "Na va vasso samuppanno,",
  "anya-e1007n0:386": "Khīṇo purāṇahāyano;",
  "anya-e1007n0:387": "Navavasse navā mettā,",
  "anya-e1007n0:388": "Bhāvitabbā hitesinā.",
  "anya-e1007n0:389": "19.",
  "anya-e1007n0:390": "Santāpayanti kamayāpyabhujaṃ na rogā,",
  "anya-e1007n0:391": "Dummantinaṃ kamupayanti na nītidosā;",
  "anya-e1007n0:392": "Kaṃ srī na mānayati kaṃ na ca hanti maccu,",
  "anya-e1007n0:393": "Kaṃ thīkatā na visayā paripīḷayanti.",
  "anya-e1007n0:394": "(Vasantatilakāgāthā.)",
  "anya-e1007n0:395": "20.",
  "anya-e1007n0:396": "(Ka) byāmamattena daṇḍena,",
  "anya-e1007n0:397": "Yolumbya udakaṃ mine;",
  "anya-e1007n0:398": "Agambhīraṃ gambhīraṃvā,",
  "anya-e1007n0:399": "Agādhe maññi gambhīraṃ.",
  "anya-e1007n0:400": "(Kha)",
  "anya-e1007n0:401": "Tathā mando saññāṇena,",
  "anya-e1007n0:402": "Agādhe maññi paṇḍitaṃ;",
  "anya-e1007n0:403": "Samāsamaṃ na jānāti,",
  "anya-e1007n0:404": "Bahvappaṃ tikkhamandataṃ.",
  "anya-e1007n0:405": "21.",
  "anya-e1007n0:406": "Duṭṭhakamme saṅgamanti,",
  "anya-e1007n0:407": "Chekakamme ca no idha;",
  "anya-e1007n0:408": "Maccuṃ vahanti sīsena,",
  "anya-e1007n0:409": "Te muḷhā muḷhasaṅgamā.",
  "anya-e1007n0:410": "22.",
  "anya-e1007n0:411": "Duvidho saṅgamo loke,",
  "anya-e1007n0:412": "Ujuko kuṭilo bhave;",
  "anya-e1007n0:413": "Ujukova pasaṃseyyo,",
  "anya-e1007n0:414": "Nohyañño sājasaṅgamo.",
  "anya-e1007n0:415": "Te iminā upāyena samaggā sammodamānā mahā bhittipiṭṭhikāya vasanti. (Mahosadhajātaka aṭṭhakathā)",
  "anya-e1007n0:416": "23.",
  "anya-e1007n0:417": "Yūthikā pupphate nohi,",
  "anya-e1007n0:418": "Siñcitāpi punappunaṃ;",
  "anya-e1007n0:419": "Pupphate sampatte kāle,",
  "anya-e1007n0:420": "Evaṃ dhāretha vīriyaṃ.",
  "anya-e1007n0:421": "24.",
  "anya-e1007n0:422": "Dhanuccayo dhanakkhepo,",
  "anya-e1007n0:423": "Duvidhā hi dhanākati;",
  "anya-e1007n0:424": "Dhanuccaye nayo atthi,",
  "anya-e1007n0:425": "Dhanakkhepamhi no idha.",
  "anya-e1007n0:426": "25.",
  "anya-e1007n0:427": "Amātā pitarasaṃ vaḍḍhaṃ,",
  "anya-e1007n0:428": "Jūtakārañca cañcalaṃ;",
  "anya-e1007n0:429": "Nālapeyya visesaññū,",
  "anya-e1007n0:430": "Yadicche siddhi mattano.",
  "anya-e1007n0:431": "Haliddirāgaṃ kapicittaṃ,",
  "anya-e1007n0:432": "Purisaṃ rāgavirāginaṃ;",
  "anya-e1007n0:433": "Edisaṃ tāta māsevi,",
  "anya-e1007n0:434": "Nimmanussaṃpi ce siyā.",
  "anya-e1007n0:435": "26.",
  "anya-e1007n0:436": "Guṇā guṇaññūsu guṇā bhavanti,",
  "anya-e1007n0:437": "Te nigguṇaṃ patvā bhavanti dosā;",
  "anya-e1007n0:438": "Āsādyatoyā pabhavanti najjo,",
  "anya-e1007n0:439": "Samudramāsajja bhavantyapeyyā.",
  "anya-e1007n0:440": "(Upajātigāthā)",
  "anya-e1007n0:441": "27.",
  "anya-e1007n0:442": "Silārūpaṃ nimminanti,",
  "anya-e1007n0:443": "Koṭṭetvāna punappunaṃ;",
  "anya-e1007n0:444": "Koṭṭakova tathā bālā,",
  "anya-e1007n0:445": "Sādhuṃ ovajja nimmitā.",
  "anya-e1007n0:446": "Cāṇakyanītilā gāthā",
  "anya-e1007n0:447": "Lālane bahavo dosā,",
  "anya-e1007n0:448": "Tāḷane bahavo guṇā;",
  "anya-e1007n0:449": "Tasmā puttañca sissañca,",
  "anya-e1007n0:450": "Tāḷaye na ca lālaye.",
  "anya-e1007n0:451": "28.",
  "anya-e1007n0:452": "Atītassa hi mittassa,",
  "anya-e1007n0:453": "Yo ce dosaṃ pakāsaye;",
  "anya-e1007n0:454": "So have paccuppannassa,",
  "anya-e1007n0:455": "Dosaṃ bhāseti ñāyati.",
  "anya-e1007n0:456": "29.",
  "anya-e1007n0:457": "Latāviya sevakā te,",
  "anya-e1007n0:458": "Ye nissayaṃ palambare;",
  "anya-e1007n0:459": "Nissayassa vināsena,",
  "anya-e1007n0:460": "Bhūmyaṃ senti anāthakā.",
  "anya-e1007n0:461": "30.",
  "anya-e1007n0:462": "Dosasiṅgehi vijjhanto,",
  "anya-e1007n0:463": "Mānakhūrehi akkamaṃ;",
  "anya-e1007n0:464": "Bhayaṃ karoti lokamhi,",
  "anya-e1007n0:465": "Gova bālo vihiṃsako.",
  "anya-e1007n0:466": "31.",
  "anya-e1007n0:467": "Ādo upari lokoyaṃ,",
  "anya-e1007n0:468": "Ujulekhāya tiṭṭhati;",
  "anya-e1007n0:469": "Musāvātehi taṃlokaṃ,",
  "anya-e1007n0:470": "Nipātesi anajjavaṃ.",
  "anya-e1007n0:471": "32.",
  "anya-e1007n0:472": "Sughaṭaṃ kumbhakārena,",
  "anya-e1007n0:473": "Nāraho paribhuñjituṃ;",
  "anya-e1007n0:474": "Tathūpamāya vekkheyya,",
  "anya-e1007n0:475": "Sakammaparakammani.",
  "anya-e1007n0:476": "33.",
  "anya-e1007n0:477": "Anantaraṃsī sūropi,",
  "anya-e1007n0:478": "Nasakkoti ghanaṃ tamaṃ;",
  "anya-e1007n0:479": "Vijjhituṃ raṃsiyā loke,",
  "anya-e1007n0:480": "Tathā madanamohitā;",
  "anya-e1007n0:481": "Nasakkonti madaṃ bhetvā,",
  "anya-e1007n0:482": "Paññābhāya pabhāsituṃ.",
  "anya-e1007n0:483": "34.",
  "anya-e1007n0:484": "Khedaveraṃ daliddamhi,",
  "anya-e1007n0:485": "Bhogimhi rogupaddavaṃ;",
  "anya-e1007n0:486": "Dessaverañca āṇimhi,",
  "anya-e1007n0:487": "Passe lokassa veritaṃ.",
  "anya-e1007n0:488": "35.",
  "anya-e1007n0:489": "Saṃladdhena subhogena,",
  "anya-e1007n0:490": "Jīvaṃ suddhaṃ kare nijaṃ;",
  "anya-e1007n0:491": "Seṭṭho so tena jīvena,",
  "anya-e1007n0:492": "Jeguccho malajīviko.",
  "anya-e1007n0:493": "36.",
  "anya-e1007n0:494": "Vajira puppharāgānaṃ,",
  "anya-e1007n0:495": "Visesaṃ yo nabujjhati;",
  "anya-e1007n0:496": "Kathañhi so vikkīṇeyya,",
  "anya-e1007n0:497": "Kīṇeyya vā yathātathaṃ.",
  "anya-e1007n0:498": "37.",
  "anya-e1007n0:499": "Kippīli kopi cintetvā,",
  "anya-e1007n0:500": "Pabbataṃ bhettu mussahaṃ;",
  "anya-e1007n0:501": "Abalā tanumajjhattā,",
  "anya-e1007n0:502": "Cintā hasyāva sā mudhā.",
  "anya-e1007n0:503": "38.",
  "anya-e1007n0:504": "Jātamattaṃ na yo sattuṃ,",
  "anya-e1007n0:505": "Rogañcūpasamaṃ naye;",
  "anya-e1007n0:506": "Mahābalopi teneva,",
  "anya-e1007n0:507": "Vuddhiṃpatvā sa haññate.",
  "anya-e1007n0:508": "39.",
  "anya-e1007n0:509": "Sajīvamaṃsabhakkhehi,",
  "anya-e1007n0:510": "Sadāṭhīhi mukhehi bho;",
  "anya-e1007n0:511": "Biḷārabyagghasīhānaṃ,",
  "anya-e1007n0:512": "Nihīnāni anekadhā;",
  "anya-e1007n0:513": "Tikkhāni kharavādāni,",
  "anya-e1007n0:514": "Manussānaṃ mukhāni ve.",
  "anya-e1007n0:515": "40.",
  "anya-e1007n0:516": "Viluppantā vidhāvanti,",
  "anya-e1007n0:517": "Sajīvavuttikammunā;",
  "anya-e1007n0:518": "Janā tena vihaññanti,",
  "anya-e1007n0:519": "Caranti dhammavemukhā.",
  "anya-e1007n0:520": "41.",
  "anya-e1007n0:521": "Sulabhaṃ lokiyaṃ loke,",
  "anya-e1007n0:522": "Sāsanīyaṃva dullabhaṃ;",
  "anya-e1007n0:523": "Dullabhaṃ taṃ vamaññanto,",
  "anya-e1007n0:524": "Eso bālatamo bhave.",
  "anya-e1007n0:525": "42.",
  "anya-e1007n0:526": "Yo patittha agyāvāṭaṃ,",
  "anya-e1007n0:527": "Mohā taṃ upakārituṃ;",
  "anya-e1007n0:528": "Aññorohi tadā vāṭaṃ,",
  "anya-e1007n0:529": "Dutīyo muḷhamuḷhako.",
  "anya-e1007n0:530": "43.",
  "anya-e1007n0:531": "Byaggho āvudhaviddho hi,",
  "anya-e1007n0:532": "Akā duṭṭhāni ninnadaṃ;",
  "anya-e1007n0:533": "Tatheva sādhusatthena,",
  "anya-e1007n0:534": "Viddho bālo pakuppito.",
  "anya-e1007n0:535": "44.",
  "anya-e1007n0:536": "Pivanti lohitaṃ ḍaṃsā,",
  "anya-e1007n0:537": "Anto tuṇḍena makkhikā;",
  "anya-e1007n0:538": "Bahiddhā parivārenti,",
  "anya-e1007n0:539": "Jano tena ḍaṃsāyaye.",
  "anya-e1007n0:540": "45.",
  "anya-e1007n0:541": "Adhanassa khaṇo appo,",
  "anya-e1007n0:542": "Saddhammo appakālino;",
  "anya-e1007n0:543": "Appako tena yuñjeyyuṃ,",
  "anya-e1007n0:544": "Khaṇaṃ bahuṃ labhetave."
}