{
  "anya-e1007n0:0": " Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1007n0:1": "Sūrassatīnīti",
  "anya-e1007n0:2": "Paṭhamo bhāgo",
  "anya-e1007n0:3": "Paṇāmagāthā",
  "anya-e1007n0:4": "1.",
  "anya-e1007n0:5": " Mukhamhā bhagavantassa,",
  "anya-e1007n0:6": "Sugandhakamalā subhā;",
  "anya-e1007n0:7": "Sañjātaṁ uttamaṁ vāṇiṁ,",
  "anya-e1007n0:8": "Vandāmi vittamānasā.",
  "anya-e1007n0:9": "2.",
  "anya-e1007n0:10": " Samajjhiṭṭho racissāmi,",
  "anya-e1007n0:11": "Dakkhena rājamantinā;",
  "anya-e1007n0:12": "Nāmena santena,",
  "anya-e1007n0:13": "Nītiṁ lokahitāvahaṁ;",
  "anya-e1007n0:14": "Sālaṅkāraṁ sopadesaṁ,",
  "anya-e1007n0:15": "Nāmenāhaṁ svarassatiṁ.",
  "anya-e1007n0:16": "3.",
  "anya-e1007n0:17": " Pekkhantvimaṁ nītiṁ santā,",
  "anya-e1007n0:18": "Mañjūsarukkhasannibhaṁ;",
  "anya-e1007n0:19": "Nānopadesasaṁpuṇṇaṁ,",
  "anya-e1007n0:20": "Sadatthakusalā sadā."
}