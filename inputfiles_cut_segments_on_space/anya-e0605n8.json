{
  "anya-e0605n8:0": "Agāriyasampattidīpanīgāthā",
  "anya-e0605n8:1": "46.",
  "anya-e0605n8:2": "Kālakkamena cando va vaḍḍhanto vaḍḍhite kule,",
  "anya-e0605n8:3": "Puññodayenudento so bhāṇumā viya ambare.",
  "anya-e0605n8:4": "47.",
  "anya-e0605n8:5": "Siddhathako hi siddhattho laddhā deviṁ yasodharaṁ,",
  "anya-e0605n8:6": "Cattālīsasahassehi pūritthīhi purakkhito.",
  "anya-e0605n8:7": "48.",
  "anya-e0605n8:8": "Rammasurammasubhesu gharesu,",
  "anya-e0605n8:9": "Tiṇṇamutūnamanucchavikesu;",
  "anya-e0605n8:10": "Dibbasukhaṁ viya bhuñji sukhaṁ so,",
  "anya-e0605n8:11": "Acchariyabbhutarājavibhūtiṁ."
}