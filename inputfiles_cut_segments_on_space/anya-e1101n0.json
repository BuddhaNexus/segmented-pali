{
  "anya-e1101n0:0": "Namo tassa bhagavato arahato sammāsambuddhassa.",
  "anya-e1101n0:1": "Rasavāhinī",
  "anya-e1101n0:2": "Paṇāmādikathā",
  "anya-e1101n0:3": "1.",
  "anya-e1101n0:4": " Satthuppasatthacaraṇaṁ saraṇaṁ janānaṁ,",
  "anya-e1101n0:5": "Brahmādimoḷi maṇiraṁsi samāvahantaṁ,",
  "anya-e1101n0:6": "Paṅkeruhābhamudukomalacāruvaṇṇaṁ;",
  "anya-e1101n0:7": "Vandāmi cakkavaralakkhaṇamādadhānaṁ.",
  "anya-e1101n0:8": "2.",
  "anya-e1101n0:9": " Siddhaṁ jinena cirakālamatanditena,",
  "anya-e1101n0:10": "Yaṁ bhāvako samadhigacchati khemamaggaṁ;",
  "anya-e1101n0:11": "Yaṁ kapparukkha rucidāna maṇiva bhāti,",
  "anya-e1101n0:12": "Taṁ dhammamagga masamaṁ paṇamāmi niccaṁ.",
  "anya-e1101n0:13": "3.",
  "anya-e1101n0:14": " Santindriyaṁ sugatasūnuvaraṁ visuddhaṁ,",
  "anya-e1101n0:15": "Yaṁ dakkhiṇeyyamatadaṁ sucipuññakhettaṁ;",
  "anya-e1101n0:16": "Tāṇesinaṁ saraṇamujjhitasabbadukkhaṁ,",
  "anya-e1101n0:17": "Vandāmi saṅgha managhaṁ sirasā mahagghaṁ.",
  "anya-e1101n0:18": "4.",
  "anya-e1101n0:19": " Yampattamettha ratanattayathomanena,",
  "anya-e1101n0:20": "Puññena tena duritaṁ sakalaṁ paṇujja,",
  "anya-e1101n0:21": "Vakkhāmahaṁ sumadhuraṁ rasavāhinintaṁ,",
  "anya-e1101n0:22": "Bho bho suṇantu sujanā bhimudāvahā sā.",
  "anya-e1101n0:23": "5.",
  "anya-e1101n0:24": " Tatthatatthūpapannāni, vatthūni arahā pure;",
  "anya-e1101n0:25": "Abhāsuṁ dīpabhāsāya, ṭhapesuṁ taṁ purātanā.",
  "anya-e1101n0:26": "6.",
  "anya-e1101n0:27": " Mahāvihāre taṅgutta, vaṅkapariveṇavāsiko;",
  "anya-e1101n0:28": "Raṭṭhapāloti nāmena, sīlācāra guṇākaro.",
  "anya-e1101n0:29": "7.",
  "anya-e1101n0:30": " Hitāya parivattesi, pajānaṁ pāḷibhāsato;",
  "anya-e1101n0:31": "Punaruttādidosehi, tamāsi sabbamākulaṁ;",
  "anya-e1101n0:32": "Anākulaṁ karissāmi, taṁ suṇātha samāhitā.",
  "anya-e1101n0:33": "8.",
  "anya-e1101n0:34": " Vitarāgā pure vocuṁ, yasmā tasmā hi bhāsitaṁ;",
  "anya-e1101n0:35": "Etamādaraṇīyañhi, sādhu sādhūhi sabbadāti.",
  "anya-e1101n0:36": "Jambudīpuppatti vatthūni."
}