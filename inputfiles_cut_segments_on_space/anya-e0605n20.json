{
  "anya-e0605n20:0": "Byāpetādiyamakagāthā",
  "anya-e0605n20:1": "110.",
  "anya-e0605n20:2": " Ekantameva saparatthaparo mahesi,",
  "anya-e0605n20:3": "Ekantameva dasapāramitābalena;",
  "anya-e0605n20:4": "Ekantameva hatamārabalena tena,",
  "anya-e0605n20:5": "Ekantameva suvisuddhamalattha bodhiṁ."
}