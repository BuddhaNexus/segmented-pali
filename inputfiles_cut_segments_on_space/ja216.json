{
  "ja216:0.1": "Jātaka ",
  "ja216:0.2": "Dukanipāta ",
  "ja216:0.3": "Bīraṇathambhavagga ",
  "ja216:0.4": "6. Macchajātaka ",
  "ja216:1.1": "“Na māyamaggi tapati, ",
  "ja216:1.2": "na sūlo sādhutacchito; ",
  "ja216:1.3": "Yañca maṁ maññate macchī, ",
  "ja216:1.4": "aññaṁ so ratiyā gato. ",
  "ja216:2.1": "So maṁ dahati rāgaggi, ",
  "ja216:2.2": "cittaṁ cūpatapeti maṁ; ",
  "ja216:2.3": "Jālino muñcathāyirā maṁ, ",
  "ja216:2.4": "na kāme haññate kvacī”ti. ",
  "ja216:3.1": "Macchajātakaṁ chaṭṭhaṁ. "
}