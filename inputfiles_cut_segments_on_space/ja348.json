{
  "ja348:0.1": "Jātaka ",
  "ja348:0.2": "Catukkanipāta ",
  "ja348:0.3": "Cūḷakuṇālavagga ",
  "ja348:0.4": "8. Araññajātaka ",
  "ja348:1.1": "“Araññā gāmamāgamma, ",
  "ja348:1.2": "kiṁsīlaṁ kiṁvataṁ ahaṁ; ",
  "ja348:1.3": "Purisaṁ tāta seveyyaṁ, ",
  "ja348:1.4": "taṁ me akkhāhi pucchito”. ",
  "ja348:2.1": "“Yo taṁ vissāsaye tāta, ",
  "ja348:2.2": "vissāsañca khameyya te; ",
  "ja348:2.3": "Sussūsī ca titikkhī ca, ",
  "ja348:2.4": "taṁ bhajehi ito gato. ",
  "ja348:3.1": "Yassa kāyena vācāya, ",
  "ja348:3.2": "manasā natthi dukkaṭaṁ; ",
  "ja348:3.3": "Urasīva patiṭṭhāya, ",
  "ja348:3.4": "taṁ bhajehi ito gato. ",
  "ja348:4.1": "Haliddirāgaṁ kapicittaṁ, ",
  "ja348:4.2": "purisaṁ rāgavirāginaṁ; ",
  "ja348:4.3": "Tādisaṁ tāta mā sevi, ",
  "ja348:4.4": "nimmanussampi ce siyā”ti. ",
  "ja348:5.1": "Araññajātakaṁ aṭṭhamaṁ. "
}