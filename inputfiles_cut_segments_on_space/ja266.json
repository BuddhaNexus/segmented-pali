{
  "ja266:0.1": "Jātaka ",
  "ja266:0.2": "Tikanipāta ",
  "ja266:0.3": "Padumavagga ",
  "ja266:0.4": "6. Vātaggasindhavajātaka ",
  "ja266:1.1": "“Yenāsi kisiyā paṇḍu, ",
  "ja266:1.2": "yena bhattaṁ na ruccati; ",
  "ja266:1.3": "Ayaṁ so āgato bhattā, ",
  "ja266:1.4": "kasmā dāni palāyasi”. ",
  "ja266:2.1": "“Sace panādikeneva, ",
  "ja266:2.2": "santhavo nāma jāyati; ",
  "ja266:2.3": "Yaso hāyati itthīnaṁ, ",
  "ja266:2.4": "tasmā tāta palāyahaṁ”. ",
  "ja266:3.1": "Yasassinaṁ kule jātaṁ, ",
  "ja266:3.2": "āgataṁ yā na icchati; ",
  "ja266:3.3": "Socati cirarattāya, ",
  "ja266:3.4": "vātaggamiva bhaddalīti. ",
  "ja266:4.1": "Vātaggasindhavajātakaṁ chaṭṭhaṁ. "
}