{
  "ja373:0.1": "Jātaka ",
  "ja373:0.2": "Pañcakanipāta ",
  "ja373:0.3": "Aḍḍhavagga ",
  "ja373:0.4": "3. Mūsikajātaka ",
  "ja373:1.1": "“Kuhiṁ gatā kattha gatā, ",
  "ja373:1.2": "iti lālappatī jano; ",
  "ja373:1.3": "Ahameveko jānāmi, ",
  "ja373:1.4": "udapāne mūsikā hatā. ",
  "ja373:2.1": "Yañcetaṁ iti cīti ca, ",
  "ja373:2.2": "gadrabhova nivattasi; ",
  "ja373:2.3": "Udapāne mūsikaṁ hantvā, ",
  "ja373:2.4": "yavaṁ bhakkhetumicchasi. ",
  "ja373:3.1": "Daharo cāsi dummedha, ",
  "ja373:3.2": "paṭhamuppattiko susu; ",
  "ja373:3.3": "Dīghañcetaṁ samāsajja, ",
  "ja373:3.4": "na te dassāmi jīvitaṁ. ",
  "ja373:4.1": "Nāntalikkhabhavanena, ",
  "ja373:4.2": "nāṅgaputtapinena vā; ",
  "ja373:4.3": "Puttena hi patthayito, ",
  "ja373:4.4": "silokehi pamocito. ",
  "ja373:5.1": "Sabbaṁ sutamadhīyetha, ",
  "ja373:5.2": "hīnamukkaṭṭhamajjhimaṁ; ",
  "ja373:5.3": "Sabbassa atthaṁ jāneyya, ",
  "ja373:5.4": "na ca sabbaṁ payojaye; ",
  "ja373:5.5": "Hoti tādisako kālo, ",
  "ja373:5.6": "yattha atthāvahaṁ sutan”ti. ",
  "ja373:6.1": "Mūsikajātakaṁ tatiyaṁ. "
}