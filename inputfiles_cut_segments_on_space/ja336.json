{
  "ja336:0.1": "Jātaka ",
  "ja336:0.2": "Catukkanipāta ",
  "ja336:0.3": "Kokilavagga ",
  "ja336:0.4": "6. Brahāchattajātaka ",
  "ja336:1.1": "“Tiṇaṁ tiṇanti lapasi, ",
  "ja336:1.2": "ko nu te tiṇamāhari; ",
  "ja336:1.3": "Kiṁ nu te tiṇakiccatthi, ",
  "ja336:1.4": "tiṇameva pabhāsasi”. ",
  "ja336:2.1": "“Idhāgamā brahmacārī, ",
  "ja336:2.2": "brahā chatto bahussuto; ",
  "ja336:2.3": "So me sabbaṁ samādāya, ",
  "ja336:2.4": "tiṇaṁ nikkhippa gacchati”. ",
  "ja336:3.1": "“Evetaṁ hoti kattabbaṁ, ",
  "ja336:3.2": "appena bahumicchatā; ",
  "ja336:3.3": "Sabbaṁ sakassa ādānaṁ, ",
  "ja336:3.4": "anādānaṁ tiṇassa ca”. ",
  "ja336:3.5": "(…) ",
  "ja336:4.1": "“Sīlavanto na kubbanti, ",
  "ja336:4.2": "bālo sīlāni kubbati; ",
  "ja336:4.3": "Aniccasīlaṁ dussīlyaṁ, ",
  "ja336:4.4": "kiṁ paṇḍiccaṁ karissatī”ti. ",
  "ja336:5.1": "Brahāchattajātakaṁ chaṭṭhaṁ. "
}