{
  "anya-e0605n25:0": "Pāṭihāriyadīpanīgāthā",
  "anya-e0605n25:1": "175.",
  "anya-e0605n25:2": " Brahmassa saddaṁ karavīkabhāṇiṁ,",
  "anya-e0605n25:3": "Yathicchitaṁ sāvayituṁ samatthaṁ;",
  "anya-e0605n25:4": "Saccaṁ piyaṁ bhūtahitaṁ vadantaṁ,",
  "anya-e0605n25:5": "Na pūjaye ko hi naro sacetano.",
  "anya-e0605n25:6": "176.",
  "anya-e0605n25:7": " Iddhi ca ādesanānusāsanī,",
  "anya-e0605n25:8": "Pāṭihīre bhagavā vasī ahu;",
  "anya-e0605n25:9": "Katvāna accherasupāṭihīraṁ,",
  "anya-e0605n25:10": "Desesi dhammaṁ anukampimaṁ pajaṁ."
}