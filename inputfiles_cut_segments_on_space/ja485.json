{
  "ja485:0.1": "Jātaka ",
  "ja485:0.2": "Pakiṇṇakanipāta ",
  "ja485:0.3": "Sālikedāravagga ",
  "ja485:0.4": "2. Candakinnarījātaka ",
  "ja485:1.1": "“Upanīyatidaṁ maññe, ",
  "ja485:1.2": "cande lohitamaddane; ",
  "ja485:1.3": "Ajja jahāmi jīvitaṁ, ",
  "ja485:1.4": "pāṇā me cande nirujjhanti. ",
  "ja485:2.1": "Osīdi me dukkhaṁ hadayaṁ, ",
  "ja485:2.2": "Me ḍayhate nitammāmi; ",
  "ja485:2.3": "Tava candiyā socantiyā, ",
  "ja485:2.4": "Na naṁ aññehi sokehi. ",
  "ja485:3.1": "Tiṇamiva vanamiva milāyāmi, ",
  "ja485:3.2": "Nadī aparipuṇṇāva sussāmi; ",
  "ja485:3.3": "Tava candiyā socantiyā, ",
  "ja485:3.4": "Na naṁ aññehi sokehi. ",
  "ja485:4.1": "Vassamiva sare pāde, ",
  "ja485:4.2": "Imāni assūni vattare mayhaṁ; ",
  "ja485:4.3": "Tava candiyā socantiyā, ",
  "ja485:4.4": "Na naṁ aññehi sokehi”. ",
  "ja485:5.1": "“Pāpo khosi rājaputta, ",
  "ja485:5.2": "Yo me icchitaṁ patiṁ varākiyā; ",
  "ja485:5.3": "Vijjhasi vanamūlasmiṁ, ",
  "ja485:5.4": "Soyaṁ viddho chamā seti. ",
  "ja485:6.1": "Imaṁ mayhaṁ hadayasokaṁ, ",
  "ja485:6.2": "Paṭimuñcatu rājaputta tava mātā; ",
  "ja485:6.3": "Yo mayhaṁ hadayasoko, ",
  "ja485:6.4": "Kimpurisaṁ avekkhamānāya. ",
  "ja485:7.1": "Imaṁ mayhaṁ hadayasokaṁ, ",
  "ja485:7.2": "Paṭimuñcatu rājaputta tava jāyā; ",
  "ja485:7.3": "Yo mayhaṁ hadayasoko, ",
  "ja485:7.4": "Kimpurisaṁ avekkhamānāya. ",
  "ja485:8.1": "Mā ca puttaṁ mā ca patiṁ, ",
  "ja485:8.2": "Addakkhi rājaputta tava mātā; ",
  "ja485:8.3": "Yo kimpurisaṁ avadhi, ",
  "ja485:8.4": "Adūsakaṁ mayha kāmā hi. ",
  "ja485:9.1": "Mā ca puttaṁ mā ca patiṁ, ",
  "ja485:9.2": "Addakkhi rājaputta tava jāyā; ",
  "ja485:9.3": "Yo kimpurisaṁ avadhi, ",
  "ja485:9.4": "Adūsakaṁ mayha kāmā hi”. ",
  "ja485:10.1": "“Mā tvaṁ cande rodi, ",
  "ja485:10.2": "Mā soci vanatimiramattakkhi; ",
  "ja485:10.3": "Mama tvaṁ hehisi bhariyā, ",
  "ja485:10.4": "Rājakule pūjitā nārībhi”. ",
  "ja485:11.1": "“Api nūnahaṁ marissaṁ, ",
  "ja485:11.2": "Nāhaṁ rājaputta tava hessaṁ; ",
  "ja485:11.3": "Yo kimpurisaṁ avadhi, ",
  "ja485:11.4": "Adūsakaṁ mayha kāmā hi”. ",
  "ja485:12.1": "“Api bhīruke api jīvitukāmike, ",
  "ja485:12.2": "Kimpurisi gaccha himavantaṁ; ",
  "ja485:12.3": "Tālīsatagarabhojanā, ",
  "ja485:12.4": "Aññe taṁ migā ramissanti”. ",
  "ja485:13.1": "“Te pabbatā tā ca kandarā, ",
  "ja485:13.2": "Tā ca giriguhāyo tatheva tiṭṭhanti; ",
  "ja485:13.3": "Tattheva taṁ apassantī, ",
  "ja485:13.4": "Kimpurisa kathaṁ ahaṁ kassaṁ. ",
  "ja485:14.1": "Te paṇṇasanthatā ramaṇīyā, ",
  "ja485:14.2": "Vāḷamigehi anuciṇṇā; ",
  "ja485:14.3": "Tattheva taṁ apassantī, ",
  "ja485:14.4": "Kimpurisa kathaṁ ahaṁ kassaṁ. ",
  "ja485:15.1": "Te pupphasanthatā ramaṇīyā, ",
  "ja485:15.2": "Vāḷamigehi anuciṇṇā; ",
  "ja485:15.3": "Tattheva taṁ apassantī, ",
  "ja485:15.4": "Kimpurisa kathaṁ ahaṁ kassaṁ. ",
  "ja485:16.1": "Acchā savanti girivananadiyo, ",
  "ja485:16.2": "Kusumābhikiṇṇasotāyo; ",
  "ja485:16.3": "Tattheva taṁ apassantī, ",
  "ja485:16.4": "Kimpurisa kathaṁ ahaṁ kassaṁ. ",
  "ja485:17.1": "Nīlāni himavato pabbatassa, ",
  "ja485:17.2": "Kūṭāni dassanīyāni; ",
  "ja485:17.3": "Tattheva taṁ apassantī, ",
  "ja485:17.4": "Kimpurisa kathaṁ ahaṁ kassaṁ. ",
  "ja485:18.1": "Pītāni himavato pabbatassa, ",
  "ja485:18.2": "Kūṭāni dassanīyāni; ",
  "ja485:18.3": "Tattheva taṁ apassantī, ",
  "ja485:18.4": "Kimpurisa kathaṁ ahaṁ kassaṁ. ",
  "ja485:19.1": "Tambāni himavato pabbatassa, ",
  "ja485:19.2": "Kūṭāni dassanīyāni; ",
  "ja485:19.3": "Tattheva taṁ apassantī, ",
  "ja485:19.4": "Kimpurisa kathaṁ ahaṁ kassaṁ. ",
  "ja485:20.1": "Tuṅgāni himavato pabbatassa, ",
  "ja485:20.2": "Kūṭāni dassanīyāni; ",
  "ja485:20.3": "Tattheva taṁ apassantī, ",
  "ja485:20.4": "Kimpurisa kathaṁ ahaṁ kassaṁ. ",
  "ja485:21.1": "Setāni himavato pabbatassa, ",
  "ja485:21.2": "Kūṭāni dassanīyāni; ",
  "ja485:21.3": "Tattheva taṁ apassantī, ",
  "ja485:21.4": "Kimpurisa kathaṁ ahaṁ kassaṁ. ",
  "ja485:22.1": "Citrāni himavato pabbatassa, ",
  "ja485:22.2": "Kūṭāni dassanīyāni; ",
  "ja485:22.3": "Tattheva taṁ apassantī, ",
  "ja485:22.4": "Kimpurisa kathaṁ ahaṁ kassaṁ. ",
  "ja485:23.1": "Yakkhagaṇasevite gandhamādane, ",
  "ja485:23.2": "Osadhebhi sañchanne; ",
  "ja485:23.3": "Tattheva taṁ apassantī, ",
  "ja485:23.4": "Kimpurisa kathaṁ ahaṁ kassaṁ. ",
  "ja485:24.1": "Kimpurisasevite gandhamādane, ",
  "ja485:24.2": "Osadhebhi sañchanne; ",
  "ja485:24.3": "Tattheva taṁ apassantī, ",
  "ja485:24.4": "Kimpurisa kathaṁ ahaṁ kassaṁ”. ",
  "ja485:25.1": "“Vande te ayirabrahme, ",
  "ja485:25.2": "Yo me icchitaṁ patiṁ varākiyā; ",
  "ja485:25.3": "Amatena abhisiñci, ",
  "ja485:25.4": "Samāgatāsmi piyatamena. ",
  "ja485:26.1": "Vicarāma dāni girivana nadiyo, ",
  "ja485:26.2": "Kusumābhikiṇṇasotāyo; ",
  "ja485:26.3": "Nānādumavasanāyo, ",
  "ja485:26.4": "Piyaṁvadā aññamaññassā”ti. ",
  "ja485:27.1": "Candakinnarījātakaṁ dutiyaṁ. "
}