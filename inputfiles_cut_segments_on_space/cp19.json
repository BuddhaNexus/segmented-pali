{
  "cp19:0.1": "Cariyāpiṭaka ",
  "cp19:0.2": "Hatthināgavagga ",
  "cp19:0.3": "Sīlapāramī 9 ",
  "cp19:0.4": "Alīnasattucariya ",
  "cp19:1.1": "“Pañcālaraṭṭhe nagaravare, ",
  "cp19:1.2": "Kapilāyaṁ puruttame; ",
  "cp19:1.3": "Rājā jayaddiso nāma, ",
  "cp19:1.4": "Sīlaguṇamupāgato. ",
  "cp19:2.1": "Tassa rañño ahaṁ putto, ",
  "cp19:2.2": "Sutadhammo susīlavā; ",
  "cp19:2.3": "Alīnasatto guṇavā, ",
  "cp19:2.4": "Anurakkhaparijano sadā. ",
  "cp19:3.1": "Pitā me migavaṁ gantvā, ",
  "cp19:3.2": "porisādaṁ upāgami; ",
  "cp19:3.3": "So me pitumaggahesi, ",
  "cp19:3.4": "‘bhakkhosi mama mā cali’. ",
  "cp19:4.1": "Tassa taṁ vacanaṁ sutvā, ",
  "cp19:4.2": "bhīto tasitavedhito; ",
  "cp19:4.3": "Ūrukkhambho ahu tassa, ",
  "cp19:4.4": "disvāna porisādakaṁ. ",
  "cp19:5.1": "Migavaṁ gahetvā muñcassu, ",
  "cp19:5.2": "katvā āgamanaṁ puna; ",
  "cp19:5.3": "Brāhmaṇassa dhanaṁ datvā, ",
  "cp19:5.4": "pitā āmantayī mamaṁ. ",
  "cp19:6.1": "‘Rajjaṁ putta paṭipajja, ",
  "cp19:6.2": "mā pamajji puraṁ idaṁ; ",
  "cp19:6.3": "Kataṁ me porisādena, ",
  "cp19:6.4": "mama āgamanaṁ puna’. ",
  "cp19:7.1": "Mātāpitū ca vanditvā, ",
  "cp19:7.2": "nimminitvāna attanā; ",
  "cp19:7.3": "Nikkhipitvā dhanuṁ khaggaṁ, ",
  "cp19:7.4": "porisādaṁ upāgamiṁ. ",
  "cp19:8.1": "Sasatthahatthūpagataṁ, ",
  "cp19:8.2": "kadāci so tasissati; ",
  "cp19:8.3": "Tena bhijjissati sīlaṁ, ",
  "cp19:8.4": "parittāsaṁ kate mayi. ",
  "cp19:9.1": "Sīlakhaṇḍabhayā mayhaṁ, ",
  "cp19:9.2": "tassa dessaṁ na byāhariṁ; ",
  "cp19:9.3": "Mettacitto hitavādī, ",
  "cp19:9.4": "idaṁ vacanamabraviṁ. ",
  "cp19:10.1": "‘Ujjālehi mahāaggiṁ, ",
  "cp19:10.2": "papatissāmi rukkhato; ",
  "cp19:10.3": "Tvaṁ pakkakālamaññāya, ",
  "cp19:10.4": "bhakkhaya maṁ pitāmaha’. ",
  "cp19:11.1": "Iti sīlavataṁ hetu, ",
  "cp19:11.2": "nārakkhiṁ mama jīvitaṁ; ",
  "cp19:11.3": "Pabbājesiṁ cahaṁ tassa, ",
  "cp19:11.4": "sadā pāṇātipātikan”ti. ",
  "cp19:12.1": "Alīnasattucariyaṁ navamaṁ. "
}