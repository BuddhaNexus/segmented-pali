{
  "ja221:0.1": "Jātaka ",
  "ja221:0.2": "Dukanipāta ",
  "ja221:0.3": "Kāsāvavagga ",
  "ja221:0.4": "1. Kāsāvajātaka ",
  "ja221:1.1": "“Anikkasāvo kāsāvaṁ, ",
  "ja221:1.2": "yo vatthaṁ paridahissati; ",
  "ja221:1.3": "Apeto damasaccena, ",
  "ja221:1.4": "na so kāsāvamarahati. ",
  "ja221:2.1": "Yo ca vantakasāvassa, ",
  "ja221:2.2": "Sīlesu susamāhito; ",
  "ja221:2.3": "Upeto damasaccena, ",
  "ja221:2.4": "Sa ve kāsāvamarahatī”ti. ",
  "ja221:3.1": "Kāsāvajātakaṁ paṭhamaṁ. "
}