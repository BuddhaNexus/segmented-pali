{
  "ja431:0.1": "Jātaka ",
  "ja431:0.2": "Navakanipāta ",
  "ja431:0.3": "Gijjhavagga ",
  "ja431:0.4": "5. Haritacajātaka ",
  "ja431:1.1": "“Sutaṁ metaṁ mahābrahme, ",
  "ja431:1.2": "kāme bhuñjati hārito; ",
  "ja431:1.3": "Kaccetaṁ vacanaṁ tucchaṁ, ",
  "ja431:1.4": "kacci suddho iriyyasi”. ",
  "ja431:2.1": "“Evametaṁ mahārāja, ",
  "ja431:2.2": "yathā te vacanaṁ sutaṁ; ",
  "ja431:2.3": "Kummaggaṁ paṭipannosmi, ",
  "ja431:2.4": "mohaneyyesu mucchito”. ",
  "ja431:3.1": "“Adu paññā kimatthiyā, ",
  "ja431:3.2": "nipuṇā sādhucintinī; ",
  "ja431:3.3": "Yāya uppatitaṁ rāgaṁ, ",
  "ja431:3.4": "kiṁ mano na vinodaye”. ",
  "ja431:4.1": "“Cattārome mahārāja, ",
  "ja431:4.2": "loke atibalā bhusā; ",
  "ja431:4.3": "Rāgo doso mado moho, ",
  "ja431:4.4": "yattha paññā na gādhati”. ",
  "ja431:5.1": "“Arahā sīlasampanno, ",
  "ja431:5.2": "suddho carati hārito; ",
  "ja431:5.3": "Medhāvī paṇḍito ceva, ",
  "ja431:5.4": "iti no sammato bhavaṁ”. ",
  "ja431:6.1": "“Medhāvīnampi hiṁsanti, ",
  "ja431:6.2": "isiṁ dhammaguṇe rataṁ; ",
  "ja431:6.3": "Vitakkā pāpakā rāja, ",
  "ja431:6.4": "subhā rāgūpasaṁhitā”. ",
  "ja431:7.1": "“Uppannāyaṁ sarīrajo, ",
  "ja431:7.2": "Rāgo vaṇṇavidūsano tava; ",
  "ja431:7.3": "Taṁ pajaha bhaddamatthu te, ",
  "ja431:7.4": "Bahunnāsi medhāvisammato”. ",
  "ja431:8.1": "“Te andhakārake kāme, ",
  "ja431:8.2": "Bahudukkhe mahāvise; ",
  "ja431:8.3": "Tesaṁ mūlaṁ gavesissaṁ, ",
  "ja431:8.4": "Checchaṁ rāgaṁ sabandhanaṁ”. ",
  "ja431:9.1": "“Idaṁ vatvāna hārito, ",
  "ja431:9.2": "isi saccaparakkamo; ",
  "ja431:9.3": "Kāmarāgaṁ virājetvā, ",
  "ja431:9.4": "brahmalokūpago ahū”ti. ",
  "ja431:10.1": "Haritacajātakaṁ pañcamaṁ. "
}