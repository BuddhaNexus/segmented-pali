{
  "ja404:0.1": "Jātaka ",
  "ja404:0.2": "Sattakanipāta ",
  "ja404:0.3": "Kukkuvagga ",
  "ja404:0.4": "9. Kapijātaka ",
  "ja404:1.1": "“Yattha verī nivasati, ",
  "ja404:1.2": "na vase tattha paṇḍito; ",
  "ja404:1.3": "Ekarattaṁ dirattaṁ vā, ",
  "ja404:1.4": "dukkhaṁ vasati verisu. ",
  "ja404:2.1": "Diso ve lahucittassa, ",
  "ja404:2.2": "posassānuvidhīyato; ",
  "ja404:2.3": "Ekassa kapino hetu, ",
  "ja404:2.4": "yūthassa anayo kato. ",
  "ja404:3.1": "Bālova paṇḍitamānī, ",
  "ja404:3.2": "yūthassa parihārako; ",
  "ja404:3.3": "Sacittassa vasaṁ gantvā, ",
  "ja404:3.4": "sayethāyaṁ yathā kapi. ",
  "ja404:4.1": "Na sādhu balavā bālo, ",
  "ja404:4.2": "yūthassa parihārako; ",
  "ja404:4.3": "Ahito bhavati ñātīnaṁ, ",
  "ja404:4.4": "sakuṇānaṁva cetako. ",
  "ja404:5.1": "Dhīrova balavā sādhu, ",
  "ja404:5.2": "yūthassa parihārako; ",
  "ja404:5.3": "Hito bhavati ñātīnaṁ, ",
  "ja404:5.4": "tidasānaṁva vāsavo. ",
  "ja404:6.1": "Yo ca sīlañca paññañca, ",
  "ja404:6.2": "sutañcattani passati; ",
  "ja404:6.3": "Ubhinnamatthaṁ carati, ",
  "ja404:6.4": "attano ca parassa ca. ",
  "ja404:7.1": "Tasmā tuleyya mattānaṁ, ",
  "ja404:7.2": "sīlapaññāsutāmiva; ",
  "ja404:7.3": "Gaṇaṁ vā parihare dhīro, ",
  "ja404:7.4": "eko vāpi paribbaje”ti. ",
  "ja404:8.1": "Kapijātakaṁ navamaṁ. "
}