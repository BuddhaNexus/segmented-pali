{
  "ja419:0.1": "Jātaka ",
  "ja419:0.2": "Aṭṭhakanipāta ",
  "ja419:0.3": "Kaccānivagga ",
  "ja419:0.4": "3. Sulasājātaka ",
  "ja419:1.1": "“Idaṁ suvaṇṇakāyūraṁ, ",
  "ja419:1.2": "muttā veḷuriyā bahū; ",
  "ja419:1.3": "Sabbaṁ harassu bhaddante, ",
  "ja419:1.4": "mañca dāsīti sāvaya”. ",
  "ja419:2.1": "“Oropayassu kalyāṇi, ",
  "ja419:2.2": "mā bāḷhaṁ paridevasi; ",
  "ja419:2.3": "Na cāhaṁ abhijānāmi, ",
  "ja419:2.4": "ahantvā dhanamābhataṁ”. ",
  "ja419:3.1": "“Yato sarāmi attānaṁ, ",
  "ja419:3.2": "yato pattāsmi viññutaṁ; ",
  "ja419:3.3": "Na cāhaṁ abhijānāmi, ",
  "ja419:3.4": "aññaṁ piyataraṁ tayā. ",
  "ja419:4.1": "Ehi taṁ upagūhissaṁ, ",
  "ja419:4.2": "karissañca padakkhiṇaṁ; ",
  "ja419:4.3": "Na hi dāni puna atthi, ",
  "ja419:4.4": "mama tuyhañca saṅgamo”. ",
  "ja419:5.1": "“Na hi sabbesu ṭhānesu, ",
  "ja419:5.2": "puriso hoti paṇḍito; ",
  "ja419:5.3": "Itthīpi paṇḍitā hoti, ",
  "ja419:5.4": "tattha tattha vicakkhaṇā. ",
  "ja419:6.1": "Na hi sabbesu ṭhānesu, ",
  "ja419:6.2": "puriso hoti paṇḍito; ",
  "ja419:6.3": "Itthīpi paṇḍitā hoti, ",
  "ja419:6.4": "lahuṁ atthaṁ vicintikā. ",
  "ja419:7.1": "Lahuñca vata khippañca, ",
  "ja419:7.2": "nikaṭṭhe samacetayi; ",
  "ja419:7.3": "Migaṁ puṇṇāyateneva, ",
  "ja419:7.4": "sulasā sattukaṁ vadhi. ",
  "ja419:8.1": "Yodha uppatitaṁ atthaṁ, ",
  "ja419:8.2": "na khippamanubujjhati; ",
  "ja419:8.3": "So haññati mandamati, ",
  "ja419:8.4": "corova girigabbhare. ",
  "ja419:9.1": "Yo ca uppatitaṁ atthaṁ, ",
  "ja419:9.2": "khippameva nibodhati; ",
  "ja419:9.3": "Muccate sattusambādhā, ",
  "ja419:9.4": "sulasā sattukāmivā”ti. ",
  "ja419:10.1": "Sulasājātakaṁ tatiyaṁ. "
}