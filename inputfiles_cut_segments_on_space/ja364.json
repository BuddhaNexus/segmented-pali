{
  "ja364:0.1": "Jātaka ",
  "ja364:0.2": "Pañcakanipāta ",
  "ja364:0.3": "Vaṇṇārohavagga ",
  "ja364:0.4": "4. Khajjopanakajātaka ",
  "ja364:1.1": "“Ko nu santamhi pajjote, ",
  "ja364:1.2": "aggipariyesanaṁ caraṁ; ",
  "ja364:1.3": "Addakkhi ratti khajjotaṁ, ",
  "ja364:1.4": "jātavedaṁ amaññatha. ",
  "ja364:2.1": "Svassa gomayacuṇṇāni, ",
  "ja364:2.2": "abhimatthaṁ tiṇāni ca; ",
  "ja364:2.3": "Viparītāya saññāya, ",
  "ja364:2.4": "nāsakkhi pajjaletave. ",
  "ja364:3.1": "Evampi anupāyena, ",
  "ja364:3.2": "atthaṁ na labhate migo; ",
  "ja364:3.3": "Visāṇato gavaṁ dohaṁ, ",
  "ja364:3.4": "yattha khīraṁ na vindati. ",
  "ja364:4.1": "Vividhehi upāyehi, ",
  "ja364:4.2": "atthaṁ papponti māṇavā; ",
  "ja364:4.3": "Niggahena amittānaṁ, ",
  "ja364:4.4": "mittānaṁ paggahena ca. ",
  "ja364:5.1": "Senāmokkhapalābhena, ",
  "ja364:5.2": "vallabhānaṁ nayena ca; ",
  "ja364:5.3": "Jagatiṁ jagatipālā, ",
  "ja364:5.4": "āvasanti vasundharan”ti. ",
  "ja364:6.1": "Khajjopanakajātakaṁ catutthaṁ. "
}