{
  "vv38:0.1": "Vimānavatthu ",
  "vv38:0.2": "Itthivimāna ",
  "vv38:0.3": "Pāricchattakavagga ",
  "vv38:0.4": "10. Pāricchattakavimānavatthu ",
  "vv38:1.1": "“Pāricchattake koviḷāre, ",
  "vv38:1.2": "ramaṇīye manorame; ",
  "vv38:1.3": "Dibbamālaṁ ganthamānā, ",
  "vv38:1.4": "gāyantī sampamodasi. ",
  "vv38:2.1": "Tassā te naccamānāya, ",
  "vv38:2.2": "aṅgamaṅgehi sabbaso; ",
  "vv38:2.3": "Dibbā saddā niccharanti, ",
  "vv38:2.4": "savanīyā manoramā. ",
  "vv38:3.1": "Tassā te naccamānāya, ",
  "vv38:3.2": "aṅgamaṅgehi sabbaso; ",
  "vv38:3.3": "Dibbā gandhā pavāyanti, ",
  "vv38:3.4": "sucigandhā manoramā. ",
  "vv38:4.1": "Vivattamānā kāyena, ",
  "vv38:4.2": "yā veṇīsu piḷandhanā; ",
  "vv38:4.3": "Tesaṁ suyyati nigghoso, ",
  "vv38:4.4": "tūriye pañcaṅgike yathā. ",
  "vv38:5.1": "Vaṭaṁsakā vātadhutā, ",
  "vv38:5.2": "vātena sampakampitā; ",
  "vv38:5.3": "Tesaṁ suyyati nigghoso, ",
  "vv38:5.4": "tūriye pañcaṅgike yathā. ",
  "vv38:6.1": "Yāpi te sirasmiṁ mālā, ",
  "vv38:6.2": "sucigandhā manoramā; ",
  "vv38:6.3": "Vāti gandho disā sabbā, ",
  "vv38:6.4": "rukkho mañjūsako yathā. ",
  "vv38:7.1": "Ghāyase taṁ sucigandhaṁ, ",
  "vv38:7.2": "rūpaṁ passasi amānusaṁ; ",
  "vv38:7.3": "Devate pucchitācikkha, ",
  "vv38:7.4": "kissa kammassidaṁ phalan”ti. ",
  "vv38:8.1": "“Pabhassaraṁ accimantaṁ, ",
  "vv38:8.2": "vaṇṇagandhena saṁyutaṁ; ",
  "vv38:8.3": "Asokapupphamālāhaṁ, ",
  "vv38:8.4": "buddhassa upanāmayiṁ. ",
  "vv38:9.1": "Tāhaṁ kammaṁ karitvāna, ",
  "vv38:9.2": "kusalaṁ buddhavaṇṇitaṁ; ",
  "vv38:9.3": "Apetasokā sukhitā, ",
  "vv38:9.4": "sampamodāmanāmayā”ti. ",
  "vv38:10.1": "Pāricchattakavimānaṁ dasamaṁ. ",
  "vv38:11.1": "Tassuddānaṁ ",
  "vv38:12.1": "Uḷāro ucchu pallaṅko, ",
  "vv38:12.2": "latā ca guttilena ca; ",
  "vv38:12.3": "Daddallapesamallikā, ",
  "vv38:12.4": "visālakkhi pāricchattako; ",
  "vv38:12.5": "Vaggo tena pavuccatīti. ",
  "vv38:13.1": "Pāricchattakavaggo tatiyo. "
}