{
  "ja117:0.1": "Jātaka ",
  "ja117:0.2": "Ekakanipāta ",
  "ja117:0.3": "Haṁcivagga ",
  "ja117:0.4": "7. Tittirajātaka ",
  "ja117:1.1": "“Accuggatātilapatā, ",
  "ja117:1.2": "ativelaṁ pabhāsitā; ",
  "ja117:1.3": "Vācā hanati dummedhaṁ, ",
  "ja117:1.4": "tittiraṁvātivassitan”ti. ",
  "ja117:2.1": "Tittirajātakaṁ sattamaṁ. "
}