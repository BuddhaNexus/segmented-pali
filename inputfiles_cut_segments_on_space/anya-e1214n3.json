{
  "anya-e1214n3:0": "Rudhādayo",
  "anya-e1214n3:1": "101.",
  "anya-e1214n3:2": "Rudhi āvaraṇe muca mocane rica recane,",
  "anya-e1214n3:3": "Sica seke yuja yoge bhuja pālanabhojane;",
  "anya-e1214n3:4": "102.",
  "anya-e1214n3:5": "Katicchede chidi dvedhākaraṇe bhida vidāraṇe",
  "anya-e1214n3:6": "Vida lābhe lupacchede vināse lipalimpane",
  "anya-e1214n3:7": "Pisa saṁcuṇṇane hisi vihiṁsāyaṁ (rudhādayo;)"
}