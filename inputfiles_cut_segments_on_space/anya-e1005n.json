{
  "anya-e1005n:6": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1005n:7": "Lokanīti",
  "anya-e1005n:8": "1. Paṇḍitakaṇḍo",
  "anya-e1005n:9": "1.",
  "anya-e1005n:10": "Lokanītiṃ pavakkhāmi,",
  "anya-e1005n:11": "Nānāsatthasamuddhaṭaṃ;",
  "anya-e1005n:12": "Māgadheneva saṅkhepaṃ,",
  "anya-e1005n:13": "Vanditvā ratanatthayaṃ.",
  "anya-e1005n:14": "2.",
  "anya-e1005n:15": "Nītiloke purisassa sāro,",
  "anya-e1005n:16": "Mātā pitā ācariyoca mitto;",
  "anya-e1005n:17": "Tasmā hi nītiṃ puriso vijaññā,",
  "anya-e1005n:18": "Ñāṇī mahā hoti bahussutoca.",
  "anya-e1005n:19": "3.",
  "anya-e1005n:20": "Alasassa kuto sippaṃ,",
  "anya-e1005n:21": "Asippassa kuto dhanaṃ;",
  "anya-e1005n:22": "Adhanassa kuto mittaṃ,",
  "anya-e1005n:23": "Amittassa kuto sukhaṃ;",
  "anya-e1005n:24": "Asukhassa kuto puññaṃ,",
  "anya-e1005n:25": "Apuññassa kuto varaṃ.",
  "anya-e1005n:26": "4.",
  "anya-e1005n:27": "Sippā samaṃ dhanaṃ natthi,",
  "anya-e1005n:28": "Sippaṃ corā nagaṇhare;",
  "anya-e1005n:29": "Idha loke sippaṃ mittaṃ,",
  "anya-e1005n:30": "Paraloke sukhāvahaṃ.",
  "anya-e1005n:31": "5.",
  "anya-e1005n:32": "Appakaṃ nātimaññeyya,",
  "anya-e1005n:33": "Citte sutaṃ nidhāpaye;",
  "anya-e1005n:34": "Vammikodakabindūva,",
  "anya-e1005n:35": "Cirena paripūrati.",
  "anya-e1005n:36": "6.",
  "anya-e1005n:37": "Khuddoti nātimaññeyya,",
  "anya-e1005n:38": "Vijjaṃ vā sippameva vā;",
  "anya-e1005n:39": "Ekampi pariyodātaṃ,",
  "anya-e1005n:40": "Jīvitakappakāraṇaṃ.",
  "anya-e1005n:41": "7.",
  "anya-e1005n:42": "Sele sele na māṇikaṃ,",
  "anya-e1005n:43": "Gaje gaje na muttikaṃ;",
  "anya-e1005n:44": "Vane vane na candanaṃ,",
  "anya-e1005n:45": "Ṭhāne ṭhāne na paṇḍitaṃ.",
  "anya-e1005n:46": "8.",
  "anya-e1005n:47": "Paṇḍito sutasampanno,",
  "anya-e1005n:48": "Yattha atthīti ce suto;",
  "anya-e1005n:49": "Mahussāhena taṃ ṭhānaṃ,",
  "anya-e1005n:50": "Gantabbaṃva sutesinā.",
  "anya-e1005n:51": "9.",
  "anya-e1005n:52": "Sine sippaṃ sine dhanaṃ,",
  "anya-e1005n:53": "Sine pabbatamāruhaṃ;",
  "anya-e1005n:54": "Sine kāmassa kodhassa,",
  "anya-e1005n:55": "Ime pañca sine sine.",
  "anya-e1005n:56": "10.",
  "anya-e1005n:57": "Suti sammuti saṅkhyāca,",
  "anya-e1005n:58": "Yogā nīti visesakā;",
  "anya-e1005n:59": "Gandhabbā gaṇikā ceva,",
  "anya-e1005n:60": "Dhanu bedā ca pūraṇā.",
  "anya-e1005n:61": "11.",
  "anya-e1005n:62": "Tikicchā itihāsā ca,",
  "anya-e1005n:63": "Joti māyā ca chandati;",
  "anya-e1005n:64": "Ketu mantā ca saddā ca,",
  "anya-e1005n:65": "Sippāṭṭhārasakā ime.",
  "anya-e1005n:66": "12.",
  "anya-e1005n:67": "Apuṭṭho paṇḍito bherī,",
  "anya-e1005n:68": "Pajjunno me hoti pucchito;",
  "anya-e1005n:69": "Bālo puṭṭho apuṭṭhopi,",
  "anya-e1005n:70": "Bahumpi bhaṇate sadā.",
  "anya-e1005n:71": "13.",
  "anya-e1005n:72": "Potthakesu ca yaṃ sippaṃ,",
  "anya-e1005n:73": "Parahatthesu yaṃ dhanaṃ;",
  "anya-e1005n:74": "Yathākicce samuppanne,",
  "anya-e1005n:75": "Na taṃ sippaṃ na taṃ dhanaṃ.",
  "anya-e1005n:76": "14.",
  "anya-e1005n:77": "Jalappamāṇaṃ kumuddanālaṃ,",
  "anya-e1005n:78": "Kulappamāṇaṃ vinayo pamāṇaṃ;",
  "anya-e1005n:79": "Byattippamāṇaṃ kathītavākyaṃ,",
  "anya-e1005n:80": "Pathaviyā pamāṇaṃ tiṇa milātaṃ.",
  "anya-e1005n:81": "15.",
  "anya-e1005n:82": "Appassuto sutaṃ appaṃ,",
  "anya-e1005n:83": "Bahuṃ maññati mānavā;",
  "anya-e1005n:84": "Sindhūdakaṃ apassanto,",
  "anya-e1005n:85": "Kūpe toyaṃva maṇḍuko.",
  "anya-e1005n:86": "16.",
  "anya-e1005n:87": "Pathamaṃ parājaye sippaṃ,",
  "anya-e1005n:88": "Dutiyaṃ parājaye dhanaṃ;",
  "anya-e1005n:89": "Tatiyaṃ parājaye dhammaṃ,",
  "anya-e1005n:90": "Catutthaṃ kiṃ karissati.",
  "anya-e1005n:91": "17.",
  "anya-e1005n:92": "Byatta putra kimalaso,",
  "anya-e1005n:93": "Abyatto bhārahārako;",
  "anya-e1005n:94": "Byattako pūjito loke,",
  "anya-e1005n:95": "Byatta putra dine dine.",
  "anya-e1005n:96": "18.",
  "anya-e1005n:97": "Mātā verī pitā satru,",
  "anya-e1005n:98": "Kena bāle na sikkhitā;",
  "anya-e1005n:99": "Sabhāmajjhe na sobhanti,",
  "anya-e1005n:100": "Haṃsamajjhe bakoyathā.",
  "anya-e1005n:101": "19.",
  "anya-e1005n:102": "Kaṇṭakaṃ giri ko tikkhati,",
  "anya-e1005n:103": "Ko añjanaṃ migakkhikaṃ;",
  "anya-e1005n:104": "Uppathaṃ pallale ko sugandhaṃ,",
  "anya-e1005n:105": "Kula-putta-rūpo ko pavattati;",
  "anya-e1005n:106": "Sāmaṃ-bhāvo.",
  "anya-e1005n:107": "20.",
  "anya-e1005n:108": "Na rasaṃ akotambulaṃ,",
  "anya-e1005n:109": "Adhanassa, laṅkatampi;",
  "anya-e1005n:110": "Alonakantu byañjanaṃ,",
  "anya-e1005n:111": "Byākaraṇaṃ asippassa.",
  "anya-e1005n:112": "21.",
  "anya-e1005n:113": "Sussusā sutasampanno,",
  "anya-e1005n:114": "Sutāpaññāya pavaḍḍhati;",
  "anya-e1005n:115": "Paññāya atthaṃ jānāti,",
  "anya-e1005n:116": "Ñāto attho sukhāvaho.",
  "anya-e1005n:117": "22.",
  "anya-e1005n:118": "Bhojanaṃ methunaṃ niddā,",
  "anya-e1005n:119": "Goṇe posepi vijjati;",
  "anya-e1005n:120": "Vijjā viseso posassa,",
  "anya-e1005n:121": "Taṃ hīno gosamo bhave.",
  "anya-e1005n:122": "23.",
  "anya-e1005n:123": "Natthi vijjāsamaṃmittaṃ,",
  "anya-e1005n:124": "Naca byādhisamo ripu;",
  "anya-e1005n:125": "Naca attasamaṃ pemaṃ,",
  "anya-e1005n:126": "Naca kammasamaṃ balaṃ.",
  "anya-e1005n:127": "24.",
  "anya-e1005n:128": "Haṃso majjhe na kākānaṃ,",
  "anya-e1005n:129": "Sīho gunnaṃ na sobhate;",
  "anya-e1005n:130": "Gadrabhamajjhe turaṅgo,",
  "anya-e1005n:131": "Bālamajjhe ca paṇḍito.",
  "anya-e1005n:132": "25.",
  "anya-e1005n:133": "Yāvajīvampi ce bālo,",
  "anya-e1005n:134": "Paṇḍitaṃ payirupāsati;",
  "anya-e1005n:135": "Na so dhammaṃ vijānāti,",
  "anya-e1005n:136": "Dabbi sūparasaṃ yathā.",
  "anya-e1005n:137": "26.",
  "anya-e1005n:138": "Muhuttamapi ce viññū,",
  "anya-e1005n:139": "Paṇḍitaṃ payirupāsati;",
  "anya-e1005n:140": "Khippaṃ dhammaṃ vijānāti,",
  "anya-e1005n:141": "Jivhā sūparasaṃ yathā.",
  "anya-e1005n:142": "27.",
  "anya-e1005n:143": "Vinā satthaṃ na gaccheyya,",
  "anya-e1005n:144": "Sūro saṅgāmabhūmiyaṃ;",
  "anya-e1005n:145": "Paṇḍitvāddhagū vāṇijo,",
  "anya-e1005n:146": "Videsagamano tathā.",
  "anya-e1005n:147": "28.",
  "anya-e1005n:148": "Dhananāsaṃ manotāpaṃ,",
  "anya-e1005n:149": "Ghare duccaritāni ca;",
  "anya-e1005n:150": "Vañcanañca avamānaṃ,",
  "anya-e1005n:151": "Paṇḍito na pakāsaye.",
  "anya-e1005n:152": "29.",
  "anya-e1005n:153": "Pattānurūpakaṃ vākyaṃ,",
  "anya-e1005n:154": "Sabhāvarūpakaṃ piyaṃ;",
  "anya-e1005n:155": "Attānurūpakaṃ kodhaṃ,",
  "anya-e1005n:156": "Yo jānāti sa paṇḍito.",
  "anya-e1005n:157": "30.",
  "anya-e1005n:158": "A-dhanassa rasaṃ khādā,",
  "anya-e1005n:159": "A-balassa hathā narā;",
  "anya-e1005n:160": "A-paññassa vākya-kathā,",
  "anya-e1005n:161": "Ummattaka-samā ime.",
  "anya-e1005n:162": "31.",
  "anya-e1005n:163": "Anavhāyaṃ gamayanto,",
  "anya-e1005n:164": "A-pucchā bahu-bhāsako;",
  "anya-e1005n:165": "Atta-guṇaṃ pakāsento,",
  "anya-e1005n:166": "Ti-vidhaṃ hīna-lakkhaṇaṃ.",
  "anya-e1005n:167": "32.",
  "anya-e1005n:168": "Appa-rūpo bahuṃ bhāso,",
  "anya-e1005n:169": "Appa-pañño pakāsito;",
  "anya-e1005n:170": "Appa-pūro ghaṭo khobhe,",
  "anya-e1005n:171": "Appa-khīrā gāvī cathe.",
  "anya-e1005n:172": "33.",
  "anya-e1005n:173": "Maṇḍūkepi ukre sīhe,",
  "anya-e1005n:174": "Kākaggahe piye piye;",
  "anya-e1005n:175": "A-paṇḍīpi paṇḍī hutvā,",
  "anya-e1005n:176": "Dhīrā pucche vaye vaye.",
  "anya-e1005n:177": "34.",
  "anya-e1005n:178": "Maṇḍūkepi ukre sīhe,",
  "anya-e1005n:179": "Sūkarepi uhe dīpe;",
  "anya-e1005n:180": "Biḷāre sadise byagghe,",
  "anya-e1005n:181": "Sabba dhīre sippa-same.",
  "anya-e1005n:182": "35.",
  "anya-e1005n:183": "Na titti rājā dhanamhi,",
  "anya-e1005n:184": "Paṇḍitopi su-bhāsite;",
  "anya-e1005n:185": "Cakkhuṃpi piya-dassane,",
  "anya-e1005n:186": "Jale sāgaro na titti.",
  "anya-e1005n:187": "36.",
  "anya-e1005n:188": "Rūpa-yobbana-sampannā,",
  "anya-e1005n:189": "Visātha-kutha-sambhavā;",
  "anya-e1005n:190": "Vijjā-hīnā na sobhanti,",
  "anya-e1005n:191": "Nigandhā iva kiṃsukā.",
  "anya-e1005n:192": "37.",
  "anya-e1005n:193": "Hīne putto rājāmacco,",
  "anya-e1005n:194": "Bāla-putto ca paṇḍito;",
  "anya-e1005n:195": "A-dhanassa dhanaṃ bahu,",
  "anya-e1005n:196": "Purisānaṃ na maññatha.",
  "anya-e1005n:197": "38.",
  "anya-e1005n:198": "Yo sippa-lobhena,",
  "anya-e1005n:199": "Bahuṃ gaṇhāti taṃ sippaṃ;",
  "anya-e1005n:200": "Mūgova supinaṃ passaṃ,",
  "anya-e1005n:201": "Kathetumpi na ussahe.",
  "anya-e1005n:202": "39.",
  "anya-e1005n:203": "Bhijjetuṃ kumbhakāro,",
  "anya-e1005n:204": "Sobhetuṃ kumbha ghaṭṭati;",
  "anya-e1005n:205": "Na khipituṃ apāyesu,",
  "anya-e1005n:206": "Sissānaṃ vuḍḍhi-kāraṇā.",
  "anya-e1005n:207": "40.",
  "anya-e1005n:208": "Taggarañca palāsena,",
  "anya-e1005n:209": "Yo naro upanayhati;",
  "anya-e1005n:210": "Pattāpi surabhi vāyanti,",
  "anya-e1005n:211": "Evaṃ dhīrūpasevanā.",
  "anya-e1005n:212": "Paṇḍitakaṇḍo niṭṭhito.",
  "anya-e1005n:213": "Sujanakaṇḍo",
  "anya-e1005n:214": "41.",
  "anya-e1005n:215": "Sabbhireva samāsetha,",
  "anya-e1005n:216": "Sabbhi kubbetha santhavaṃ;",
  "anya-e1005n:217": "Sataṃ saddhammamaññāya,",
  "anya-e1005n:218": "Seyyo hoti na pāpiyo.",
  "anya-e1005n:219": "42.",
  "anya-e1005n:220": "Caja dujjana saṃsaggaṃ,",
  "anya-e1005n:221": "Bhaja sādhu samāgamaṃ;",
  "anya-e1005n:222": "Kara puññamahorattiṃ,",
  "anya-e1005n:223": "Sara niccamaniccataṃ.",
  "anya-e1005n:224": "43.",
  "anya-e1005n:225": "Yathā udumbarapakkā,",
  "anya-e1005n:226": "Bahirattakamevaca;",
  "anya-e1005n:227": "Anto kimīhi sampuṇṇā,",
  "anya-e1005n:228": "Evaṃ dujjanahaddayā.",
  "anya-e1005n:229": "44.",
  "anya-e1005n:230": "Yathāpi panasāpakkā,",
  "anya-e1005n:231": "Bahi kaṇḍakameva ca;",
  "anya-e1005n:232": "Anto amatasampannā,",
  "anya-e1005n:233": "Evaṃ sujanahadayā.",
  "anya-e1005n:234": "45.",
  "anya-e1005n:235": "Sukkhopi candanataru na jahāti gandhaṃ,",
  "anya-e1005n:236": "Nāgo gato naramukhe na jahāti līḷaṃ;",
  "anya-e1005n:237": "Yantāgato madhurasaṃ na jahāti ucchu,",
  "anya-e1005n:238": "Dukkhopi paṇḍitajano na jahāti dhammaṃ.",
  "anya-e1005n:239": "46.",
  "anya-e1005n:240": "Sīho nāma jighacchāpi,",
  "anya-e1005n:241": "Paṇṇādīni na khādati;",
  "anya-e1005n:242": "Sīho nāma kisocāpi,",
  "anya-e1005n:243": "Nāgamaṃsaṃ na khādati.",
  "anya-e1005n:244": "47.",
  "anya-e1005n:245": "Kula-jāto kula-putto,",
  "anya-e1005n:246": "Kula-vaṃsa-surakkhato;",
  "anya-e1005n:247": "Attanā dukkha-pattopi,",
  "anya-e1005n:248": "Hīna-kammaṃ na kāraye.",
  "anya-e1005n:249": "48.",
  "anya-e1005n:250": "Candanaṃ sītalaṃ loke,",
  "anya-e1005n:251": "Tato candaṃva sītalaṃ;",
  "anya-e1005n:252": "Canda-candanasītamhā,",
  "anya-e1005n:253": "Sādhu vākyaṃ subhāsitaṃ.",
  "anya-e1005n:254": "49.",
  "anya-e1005n:255": "Udeyya bhāṇu pacchime,",
  "anya-e1005n:256": "Merurājā nameyyapi;",
  "anya-e1005n:257": "Sītalā narakaggipi,",
  "anya-e1005n:258": "Pabbatagge ca uppalaṃ.",
  "anya-e1005n:259": "Vikase na viparītaṃ,",
  "anya-e1005n:260": "Sādhuvāyyaṃ kudācanaṃ.",
  "anya-e1005n:261": "50.",
  "anya-e1005n:262": "Sukhā rukkhassa chāyāva,",
  "anya-e1005n:263": "Tato ñāti mātā pitu;",
  "anya-e1005n:264": "Tato ācariyo rañño,",
  "anya-e1005n:265": "Tato buddhassanekadhā.",
  "anya-e1005n:266": "51.",
  "anya-e1005n:267": "Bhamarā pupphamicchanti,",
  "anya-e1005n:268": "Guṇamicchanti sujanā;",
  "anya-e1005n:269": "Makkhikā pūtimicchanti,",
  "anya-e1005n:270": "Dosamicchanti dujjanā.",
  "anya-e1005n:271": "52.",
  "anya-e1005n:272": "Mātāhīnassa dubbhāsā,",
  "anya-e1005n:273": "Pitāhīnassa dukriyā;",
  "anya-e1005n:274": "Ubho mātā pitā hīnā,",
  "anya-e1005n:275": "Dubbhasāca dukīriyā.",
  "anya-e1005n:276": "53.",
  "anya-e1005n:277": "Mātā seṭṭhassa subhāsā,",
  "anya-e1005n:278": "Pitā seṭṭhassa sukriyā;",
  "anya-e1005n:279": "Ubho mātā pitā seṭṭhā,",
  "anya-e1005n:280": "Subhāsāca sukīriyā.",
  "anya-e1005n:281": "54.",
  "anya-e1005n:282": "Saṅgāme sūra-micchanti,",
  "anya-e1005n:283": "Mantīsu akūtūhalaṃ;",
  "anya-e1005n:284": "Piyañca annapānesu,",
  "anya-e1005n:285": "Atthe jāte ca paṇḍitaṃ.",
  "anya-e1005n:286": "55.",
  "anya-e1005n:287": "Sunakho sunakhaṃ disvā,",
  "anya-e1005n:288": "Dantaṃ dasseti hiṃsituṃ;",
  "anya-e1005n:289": "Dujjano sujanaṃ disvā,",
  "anya-e1005n:290": "Rosayaṃ hiṃsamicchati.",
  "anya-e1005n:291": "56.",
  "anya-e1005n:292": "Mā ca vegena kiccāni,",
  "anya-e1005n:293": "Karosi kārāpesi vā;",
  "anya-e1005n:294": "Sahasā kāritaṃ kammaṃ,",
  "anya-e1005n:295": "Mando pacchānutappati.",
  "anya-e1005n:296": "57.",
  "anya-e1005n:297": "Kodhaṃ vihitvā na kadācinasoce,",
  "anya-e1005n:298": "Makkhappahānaṃ isayo avaṇṇayuṃ;",
  "anya-e1005n:299": "Sabbesa phārusa-vacaṃ khametha,",
  "anya-e1005n:300": "Etaṃ khantiṃ uttamamāhu santo.",
  "anya-e1005n:301": "58.",
  "anya-e1005n:302": "Dukkho nivāso sambādhe,",
  "anya-e1005n:303": "Ṭhāne asucisaṅkate;",
  "anya-e1005n:304": "Tato arimhi appiye,",
  "anya-e1005n:305": "Tatopi akataññunā.",
  "anya-e1005n:306": "59.",
  "anya-e1005n:307": "Ovādeyyānusāseyya,",
  "anya-e1005n:308": "Gāpakā ca nivāraye;",
  "anya-e1005n:309": "Satañhi so piyo hoti,",
  "anya-e1005n:310": "Asataṃ hoti appiyo.",
  "anya-e1005n:311": "60.",
  "anya-e1005n:312": "Uttamattanivātena,",
  "anya-e1005n:313": "Sūraṃ bhedena nijjaye;",
  "anya-e1005n:314": "Nīcaṃ appaka dānena,",
  "anya-e1005n:315": "Vīriyena samaṃ jaye.",
  "anya-e1005n:316": "61.",
  "anya-e1005n:317": "Na visaṃ visamiccāhu,",
  "anya-e1005n:318": "Dhanaṃ saṅghassa uccate;",
  "anya-e1005n:319": "Visaṃ ekaṃva hanati,",
  "anya-e1005n:320": "Sabbaṃ saṅghassa santakaṃ.",
  "anya-e1005n:321": "62.",
  "anya-e1005n:322": "Javane bhadraṃ jānanti,",
  "anya-e1005n:323": "Baliddañca vāhenā;",
  "anya-e1005n:324": "Duhena dhenuṃ jānanti,",
  "anya-e1005n:325": "Bhāsamānena paṇḍitaṃ.",
  "anya-e1005n:326": "63.",
  "anya-e1005n:327": "Dhanamappampi sādhūnaṃ,",
  "anya-e1005n:328": "Kūpe vāriva nissayo;",
  "anya-e1005n:329": "Bahuṃ api asādhūnaṃ,",
  "anya-e1005n:330": "Naca vārīva aṇṇave.",
  "anya-e1005n:331": "64.",
  "anya-e1005n:332": "Najjo pivanti no āpaṃ,",
  "anya-e1005n:333": "Rukkhā khādanti no phalaṃ;",
  "anya-e1005n:334": "Vassanti kvaci no meghā,",
  "anya-e1005n:335": "Paratthāya sataṃ dhanaṃ.",
  "anya-e1005n:336": "65.",
  "anya-e1005n:337": "Apattheyyaṃ na pattheyya,",
  "anya-e1005n:338": "A cinteyyaṃ na cintaye;",
  "anya-e1005n:339": "Dhammameva sucinteyya,",
  "anya-e1005n:340": "Kālaṃ moghaṃ na accaye.",
  "anya-e1005n:341": "66.",
  "anya-e1005n:342": "Acintitampi bhavati,",
  "anya-e1005n:343": "Cintitampi vinassati;",
  "anya-e1005n:344": "Na hi cintāmayā bhogā,",
  "anya-e1005n:345": "Itthiyā purisassavā.",
  "anya-e1005n:346": "67.",
  "anya-e1005n:347": "Asantassa piyo hoti,",
  "anya-e1005n:348": "Sante na kurute piyaṃ;",
  "anya-e1005n:349": "Asataṃ dhammaṃ roceti,",
  "anya-e1005n:350": "Taṃ parābhavato mukhaṃ.",
  "anya-e1005n:351": "Sujanakaṇḍo niṭṭhito.",
  "anya-e1005n:352": "4. Bāladujjanakaṇḍo",
  "anya-e1005n:353": "68.",
  "anya-e1005n:354": "Atippiyo na kātabbo,",
  "anya-e1005n:355": "Khalo kotūhalaṃ karo;",
  "anya-e1005n:356": "Sirasā vahyamānopi,",
  "anya-e1005n:357": "Aḍḍhapūro ghaṭo yathā.",
  "anya-e1005n:358": "69.",
  "anya-e1005n:359": "Sappo duṭṭho khalo duṭṭho,",
  "anya-e1005n:360": "Sappo duṭṭhataro khalo;",
  "anya-e1005n:361": "Mantosadhehi so sappo,",
  "anya-e1005n:362": "Khalo kenupasammati.",
  "anya-e1005n:363": "70.",
  "anya-e1005n:364": "Yo bālo maññati bālyaṃ,",
  "anya-e1005n:365": "Paṇḍito vāpi tena so;",
  "anya-e1005n:366": "Bālova paṇḍitamānī,",
  "anya-e1005n:367": "So ve bāloti vuccati.",
  "anya-e1005n:368": "71.",
  "anya-e1005n:369": "Madhuṃva maññatī bālo,",
  "anya-e1005n:370": "Yāva pāpaṃ na paccati;",
  "anya-e1005n:371": "Yadāca paccatī pāpaṃ,",
  "anya-e1005n:372": "Atha dukkhaṃ nigacchati.",
  "anya-e1005n:373": "72.",
  "anya-e1005n:374": "Na sādhu balavā bālo,",
  "anya-e1005n:375": "Sahasā vindate dhanaṃ;",
  "anya-e1005n:376": "Kāyassa bhedā duppañño,",
  "anya-e1005n:377": "Nirayaṃ sopapajjati.",
  "anya-e1005n:378": "73.",
  "anya-e1005n:379": "Ghare duṭṭho ca mūsīko ca,",
  "anya-e1005n:380": "Vane duṭṭho ca vānaro;",
  "anya-e1005n:381": "Sakuṇe ca duṭṭho kāko,",
  "anya-e1005n:382": "Nare duṭṭhoca brāhmaṇo.",
  "anya-e1005n:383": "74.",
  "anya-e1005n:384": "Dīghā jāgarato ratti,",
  "anya-e1005n:385": "Dīghaṃ santassa yojanaṃ;",
  "anya-e1005n:386": "Dīgho bālāna saṃsāro,",
  "anya-e1005n:387": "Saddhammaṃ a-vijānataṃ.",
  "anya-e1005n:388": "75.",
  "anya-e1005n:389": "Tila mattaṃ paresaṃva,",
  "anya-e1005n:390": "Appa dosañca passati;",
  "anya-e1005n:391": "Nāḷikerampi sadosaṃ,",
  "anya-e1005n:392": "Khala-jāto na passati.",
  "anya-e1005n:393": "76.",
  "anya-e1005n:394": "Nattadosaṃ pare jaññā,",
  "anya-e1005n:395": "Jaññā dosaṃ parassatu;",
  "anya-e1005n:396": "Guyhe kummo aṅgāni,",
  "anya-e1005n:397": "Para dosañca lakkhaye.",
  "anya-e1005n:398": "77.",
  "anya-e1005n:399": "Paṇḍitassa pasaṃsāya,",
  "anya-e1005n:400": "Daṇḍo bālena dīyate;",
  "anya-e1005n:401": "Paṇḍito paṇḍiteneva,",
  "anya-e1005n:402": "Vaṇṇitova suvaṇṇito.",
  "anya-e1005n:403": "78.",
  "anya-e1005n:404": "Luddhaṃ atthena gaṇheyya,",
  "anya-e1005n:405": "Thaddhaṃ añjali kammunā;",
  "anya-e1005n:406": "Chandānuvattiyā mūḷhaṃ,",
  "anya-e1005n:407": "Yathābhūtena paṇḍitaṃ.",
  "anya-e1005n:408": "Bāladujjanakaṇḍo niṭṭhito.",
  "anya-e1005n:409": "4. Mittakaṇḍo",
  "anya-e1005n:410": "79.",
  "anya-e1005n:411": "Hitakāro paro bandhu,",
  "anya-e1005n:412": "Bandhūpi ahito paro;",
  "anya-e1005n:413": "A hito dehajo byādhi,",
  "anya-e1005n:414": "Hitaṃ araññamosadhaṃ.",
  "anya-e1005n:415": "80.",
  "anya-e1005n:416": "Parokkhe guṇa-hantāraṃ,",
  "anya-e1005n:417": "Paccakkhe piyavādinaṃ;",
  "anya-e1005n:418": "Vajjeyya tādisaṃ mittaṃ,",
  "anya-e1005n:419": "Visakumbhe yathā madhuṃ.",
  "anya-e1005n:420": "81.",
  "anya-e1005n:421": "Dhanahīne caje mitto,",
  "anya-e1005n:422": "Puttadārā sahodarā;",
  "anya-e1005n:423": "Dhanavantaṃva sevanti,",
  "anya-e1005n:424": "Dhanaṃ loke mahā sakhā.",
  "anya-e1005n:425": "82.",
  "anya-e1005n:426": "Jāneyya pesena bhaccaṃ,",
  "anya-e1005n:427": "Bandhuṃ vāpi bhayāgate;",
  "anya-e1005n:428": "Appakāsu tathā mittaṃ,",
  "anya-e1005n:429": "Dārañca vibhavakkhaye.",
  "anya-e1005n:430": "83.",
  "anya-e1005n:431": "So bandhu yo hite yutto,",
  "anya-e1005n:432": "Pitaro honti posako;",
  "anya-e1005n:433": "Taṃ mittaṃ yattha vissāso,",
  "anya-e1005n:434": "Sā bhariyā ca yassa nibbūti.",
  "anya-e1005n:435": "84.",
  "anya-e1005n:436": "Na vissase a-vissattaṃ,",
  "anya-e1005n:437": "Mittañcāpi na vissase;",
  "anya-e1005n:438": "Kadāci kupito mito,",
  "anya-e1005n:439": "Sabbaṃ dosaṃ pakāsaye.",
  "anya-e1005n:440": "85.",
  "anya-e1005n:441": "Sakiṃ duṭṭhañca yo mittaṃ,",
  "anya-e1005n:442": "Puna sandhitumicchati;",
  "anya-e1005n:443": "So maccuṃ upagaṇhāti,",
  "anya-e1005n:444": "Gabbhamassatarī yathā.",
  "anya-e1005n:445": "86.",
  "anya-e1005n:446": "Vahe a-mittaṃ khandhena,",
  "anya-e1005n:447": "Yāva kālo anāgato;",
  "anya-e1005n:448": "Tameva āgate kāle,",
  "anya-e1005n:449": "Sele bhinde ghaṭaṃ iva.",
  "anya-e1005n:450": "87.",
  "anya-e1005n:451": "Iṇaseso aggiseso,",
  "anya-e1005n:452": "Satruseso tatheva ca;",
  "anya-e1005n:453": "Punappunaṃ vivaḍḍhanti,",
  "anya-e1005n:454": "Tasmā sesaṃ na kāraye.",
  "anya-e1005n:455": "88.",
  "anya-e1005n:456": "Padumaṃva mukhaṃ yassa,",
  "anya-e1005n:457": "Vācā candana sītalā;",
  "anya-e1005n:458": "Tādisaṃ nopaseveyya,",
  "anya-e1005n:459": "Hadayetu halāhalaṃ.",
  "anya-e1005n:460": "89.",
  "anya-e1005n:461": "Na seve pharusaṃ sāmiṃ,",
  "anya-e1005n:462": "Naca seveyya macchariṃ;",
  "anya-e1005n:463": "Tato apaggaṇhaṃ sāmiṃ,",
  "anya-e1005n:464": "Neva niggahitaṃ tato.",
  "anya-e1005n:465": "90.",
  "anya-e1005n:466": "Siṅgī paññāsa hatthena,",
  "anya-e1005n:467": "Vajje satena vājinaṃ;",
  "anya-e1005n:468": "Hatthiṃ dantiṃ sahassena,",
  "anya-e1005n:469": "Desa cāgena dujjanaṃ.",
  "anya-e1005n:470": "91.",
  "anya-e1005n:471": "Kudesañca kumittañca,",
  "anya-e1005n:472": "Kukulañca kubandhavaṃ;",
  "anya-e1005n:473": "Kudārañca kudāsañca,",
  "anya-e1005n:474": "Dūrato parivajjaye.",
  "anya-e1005n:475": "92.",
  "anya-e1005n:476": "Rogāture ca dubbhikkhe,",
  "anya-e1005n:477": "Byasane sattu viggahe;",
  "anya-e1005n:478": "Rājadvāre susāne ca,",
  "anya-e1005n:479": "Ye tiṭṭhanti sumittakā.",
  "anya-e1005n:480": "93.",
  "anya-e1005n:481": "Sītavāco bahumitto,",
  "anya-e1005n:482": "Pharuso appamittako;",
  "anya-e1005n:483": "Upamaṃ ettha ñātabbā,",
  "anya-e1005n:484": "Canda-sūriya-rājūnaṃ.",
  "anya-e1005n:485": "Mittakaṇḍo niṭṭhito.",
  "anya-e1005n:486": "5. Itthikaṇḍo",
  "anya-e1005n:487": "94.",
  "anya-e1005n:488": "Kokilānaṃ saddaṃ rūpaṃ,",
  "anya-e1005n:489": "Nārīrūpaṃ patibbatā;",
  "anya-e1005n:490": "Vijjā rūpaṃ a-rūpānaṃ,",
  "anya-e1005n:491": "Khamā rūpaṃ tapassinaṃ.",
  "anya-e1005n:492": "95.",
  "anya-e1005n:493": "Itthīnañca dhanaṃ rūpaṃ,",
  "anya-e1005n:494": "Purisānaṃ vijjā dhanaṃ;",
  "anya-e1005n:495": "Bhikkhūnañca dhanaṃ sīlaṃ,",
  "anya-e1005n:496": "Rājānañca dhanaṃ balaṃ.",
  "anya-e1005n:497": "96.",
  "anya-e1005n:498": "Tapassino kisā sobhā,",
  "anya-e1005n:499": "Thūlā sobhā catuppadā;",
  "anya-e1005n:500": "Purisā vijjavā sobhā,",
  "anya-e1005n:501": "Itthī sobhāsa sāmikā.",
  "anya-e1005n:502": "97.",
  "anya-e1005n:503": "Pañca ratyā sugandhabbā,",
  "anya-e1005n:504": "Satta ratyā dhanuggahā;",
  "anya-e1005n:505": "Eka māsā subhariyā,",
  "anya-e1005n:506": "Aḍḍha māsā sissā malā.",
  "anya-e1005n:507": "98.",
  "anya-e1005n:508": "Hiṃ ramati paṅa,",
  "anya-e1005n:509": "Haṅa ramati poka.",
  "anya-e1005n:510": "Thī ramati pu,",
  "anya-e1005n:511": "Khu ramati dhaṃ.",
  "anya-e1005n:512": "99.",
  "anya-e1005n:513": "Jiṇṇamannaṃ pasaṃseyya,",
  "anya-e1005n:514": "Dārañca gatayobbanaṃ;",
  "anya-e1005n:515": "Raṇā punāgatā sūraṃ,",
  "anya-e1005n:516": "Sassañca gehamāgataṃ.",
  "anya-e1005n:517": "100.",
  "anya-e1005n:518": "Dvatti-patikā nārī ca,",
  "anya-e1005n:519": "Bhikkhu dvatti-vihāriko;",
  "anya-e1005n:520": "Dvatti-pāsa-mutto pakkhī,",
  "anya-e1005n:521": "Kata-māyā bahūtharaṃ.",
  "anya-e1005n:522": "101.",
  "anya-e1005n:523": "Dujjanaṃ pahārādame,",
  "anya-e1005n:524": "Mittaṃ dame a-bhāṇikā;",
  "anya-e1005n:525": "Itthiñca byasanā dame,",
  "anya-e1005n:526": "Rāginaṃ appa bhojanā.",
  "anya-e1005n:527": "102.",
  "anya-e1005n:528": "Na ratti vinā candimā,",
  "anya-e1005n:529": "Vīciṃ vinā ca aṇṇavo;",
  "anya-e1005n:530": "Haṃsaṃ vinā pokkharaṇī,",
  "anya-e1005n:531": "Patiṃ kaññāca sobhate.",
  "anya-e1005n:532": "103.",
  "anya-e1005n:533": "Patinā janito bhogo,",
  "anya-e1005n:534": "Itthiyāva saṃgoppito;",
  "anya-e1005n:535": "Purisova hi padhāno,",
  "anya-e1005n:536": "Itthī suttaṃva sūciyā.",
  "anya-e1005n:537": "104.",
  "anya-e1005n:538": "Sabbānadī vaṅkanadī,",
  "anya-e1005n:539": "Sabbe kaṭṭhamayā vanā;",
  "anya-e1005n:540": "Sabbitthiyo kare pāpaṃ,",
  "anya-e1005n:541": "Labhamāne nivātake.",
  "anya-e1005n:542": "105.",
  "anya-e1005n:543": "Vivādasīliṃ usūyabhāṇiniṃ,",
  "anya-e1005n:544": "Sampassataṇhiṃ bahupākabhuttiniṃ;",
  "anya-e1005n:545": "Aggantabhuttiṃ paragehavāsiniṃ,",
  "anya-e1005n:546": "Nāriṃ caje puttasatampi pūmā.",
  "anya-e1005n:547": "106.",
  "anya-e1005n:548": "Bhuttesu maṇḍesu janīva kantinī,",
  "anya-e1005n:549": "Guyheca ṭhāne bhaginīva hiriṇī;",
  "anya-e1005n:550": "Kammesu pattesu karoti dāsīva,",
  "anya-e1005n:551": "Bhayesu mantī sayanesu rāmaye;",
  "anya-e1005n:552": "Rūpīsu sikkhī kupanesu khantinī,",
  "anya-e1005n:553": "Sā nārī seṭṭhāti vadanti paṇḍitā;",
  "anya-e1005n:554": "Kāyassa bhedāca divebhaveyya sā.",
  "anya-e1005n:555": "107.",
  "anya-e1005n:556": "Sāmā migakkhī tanumajjhagattā,",
  "anya-e1005n:557": "Sūrū sukesī samadantapantī;",
  "anya-e1005n:558": "Gambhīranābhī yuvatī susīlī,",
  "anya-e1005n:559": "Hīne kule jātāpi vivāhyā.",
  "anya-e1005n:560": "108.",
  "anya-e1005n:561": "Saradaṃratu-kālānaṃ,",
  "anya-e1005n:562": "Bhariyānaṃ rūpavatī;",
  "anya-e1005n:563": "Jeṭṭho padhānaṃ puttānaṃ,",
  "anya-e1005n:564": "Disānaṃ uttarādisā.",
  "anya-e1005n:565": "109.",
  "anya-e1005n:566": "Yā icche puriso hotuṃ,",
  "anya-e1005n:567": "Jāti jāti punappunaṃ;",
  "anya-e1005n:568": "Sāmikaṃ apacāyeyya,",
  "anya-e1005n:569": "Indaṃva pāricārikā.",
  "anya-e1005n:570": "110.",
  "anya-e1005n:571": "Yo icche puriso hotuṃ,",
  "anya-e1005n:572": "Jāti jāti punappunaṃ;",
  "anya-e1005n:573": "Paradānaṃ vivajjeyya,",
  "anya-e1005n:574": "Dhotapādova kaddamaṃ.",
  "anya-e1005n:575": "111.",
  "anya-e1005n:576": "Atikkanta vayo poso,",
  "anya-e1005n:577": "Āneti timbaruttaniṃ;",
  "anya-e1005n:578": "Tassā issā asaddhāti,",
  "anya-e1005n:579": "Taṃ parābhavato mukhaṃ.",
  "anya-e1005n:580": "Itthikaṇḍo niṭṭhito.",
  "anya-e1005n:581": "6. Rājakaṇḍo",
  "anya-e1005n:582": "112.",
  "anya-e1005n:583": "Ekayāmaṃ saye rājā,",
  "anya-e1005n:584": "Dviyāmaññeva paṇḍito;",
  "anya-e1005n:585": "Gharāvāso tiyāmaṃva,",
  "anya-e1005n:586": "Catuyāmaṃ tu yācako.",
  "anya-e1005n:587": "113.",
  "anya-e1005n:588": "Dhanavā sutavā rājā,",
  "anya-e1005n:589": "Nadī vejjo cimepañca;",
  "anya-e1005n:590": "Yattha dese na vijjanti,",
  "anya-e1005n:591": "Na tattha divasaṃ vase.",
  "anya-e1005n:592": "114.",
  "anya-e1005n:593": "Yasmiṃ padese na māno,",
  "anya-e1005n:594": "Na pemaṃ naca bandhavā;",
  "anya-e1005n:595": "Naca vijjāgamo koci,",
  "anya-e1005n:596": "Na tattha divasaṃ vase.",
  "anya-e1005n:597": "115.",
  "anya-e1005n:598": "Aputtakaṃ gharaṃ suññaṃ,",
  "anya-e1005n:599": "Raṭṭhaṃ suññaṃ arājakaṃ;",
  "anya-e1005n:600": "A sippassa mukhaṃ suññaṃ,",
  "anya-e1005n:601": "Sabba suññaṃ daliddakā.",
  "anya-e1005n:602": "116.",
  "anya-e1005n:603": "Dhanamiccheyya vāṇijjo,",
  "anya-e1005n:604": "Vijjamicche bhajesutaṃ;",
  "anya-e1005n:605": "Puttamicche taruṇitthiṃ,",
  "anya-e1005n:606": "Rājāmaccaṃ vasaṃ game.",
  "anya-e1005n:607": "117.",
  "anya-e1005n:608": "Naṭṭhoyati asantuṭṭho,",
  "anya-e1005n:609": "Santuṭṭho ca mahīpati;",
  "anya-e1005n:610": "Lajjā ca gaṇikā naṭṭhā,",
  "anya-e1005n:611": "Nillajjā kuladhītikā.",
  "anya-e1005n:612": "118.",
  "anya-e1005n:613": "Pakkhīnaṃ balamākāso,",
  "anya-e1005n:614": "Macchānamudakaṃ balaṃ;",
  "anya-e1005n:615": "Dubbalassa balaṃ rājā,",
  "anya-e1005n:616": "Kumārānaṃ rudaṃ balaṃ.",
  "anya-e1005n:617": "119.",
  "anya-e1005n:618": "Khamā jāgariyuṭṭhānaṃ,",
  "anya-e1005n:619": "Saṃvibhāgo dayikkhaṇā;",
  "anya-e1005n:620": "Nāyakassa guṇā ete,",
  "anya-e1005n:621": "Icchitabbā sataṃ guṇā.",
  "anya-e1005n:622": "120.",
  "anya-e1005n:623": "Sakiṃ vadanti rājāno,",
  "anya-e1005n:624": "Sakiṃ samaṇabrāhmaṇā;",
  "anya-e1005n:625": "Sakiṃ sappurisā loke,",
  "anya-e1005n:626": "Esa dhammo sanantano.",
  "anya-e1005n:627": "121.",
  "anya-e1005n:628": "Alaso gihī kāmabhogī na sādhu,",
  "anya-e1005n:629": "Asaññato pabbajito na sādhu;",
  "anya-e1005n:630": "Rājā anisammakārī na sādhu,",
  "anya-e1005n:631": "Paṇḍito kodhano taṃpi na sādhu.",
  "anya-e1005n:632": "122.",
  "anya-e1005n:633": "Bahavo yattha nettāro,",
  "anya-e1005n:634": "Sabbe paṇḍitamānino;",
  "anya-e1005n:635": "Sabbe mahattamicchanti,",
  "anya-e1005n:636": "Tesaṃ kammaṃ vinassati.",
  "anya-e1005n:637": "123.",
  "anya-e1005n:638": "Āyaṃ khayaṃ sayaṃ jaññā,",
  "anya-e1005n:639": "Rājā sayaṃ katākataṃ;",
  "anya-e1005n:640": "Niggahe niggahetabbaṃ,",
  "anya-e1005n:641": "Paggahe paggahārahaṃ.",
  "anya-e1005n:642": "124.",
  "anya-e1005n:643": "Piṭṭhitokkaṃ niseveyya,",
  "anya-e1005n:644": "Kucchināva hutāsanaṃ;",
  "anya-e1005n:645": "Sāmikaṃ sabbabhāgena,",
  "anya-e1005n:646": "Paralokaṃ amohavā.",
  "anya-e1005n:647": "125.",
  "anya-e1005n:648": "Aggi āpo itthimūḷho,",
  "anya-e1005n:649": "Sappo rāja-kulānica;",
  "anya-e1005n:650": "Apayantena gantabbā,",
  "anya-e1005n:651": "Acceka-pāṇahārakā.",
  "anya-e1005n:652": "126.",
  "anya-e1005n:653": "Paduṭṭha-bhariya-saṃvāso,",
  "anya-e1005n:654": "Paduṭṭha citta dāsako;",
  "anya-e1005n:655": "Sa-sappe ca ghare vāso,",
  "anya-e1005n:656": "Maccu eva na saṃsayo.",
  "anya-e1005n:657": "127.",
  "anya-e1005n:658": "Mūḷha sisso padesena,",
  "anya-e1005n:659": "Kunārī bharaṇena ca;",
  "anya-e1005n:660": "Asatā sampayogena,",
  "anya-e1005n:661": "Paṇḍitoppavasīdati.",
  "anya-e1005n:662": "128.",
  "anya-e1005n:663": "Mātā puttakaraṃ pāpaṃ,",
  "anya-e1005n:664": "Sissapāpaṃ gurukatā;",
  "anya-e1005n:665": "Rājā raṭṭhakaraṃ pāpaṃ,",
  "anya-e1005n:666": "Rājapāpaṃ purohito.",
  "anya-e1005n:667": "129.",
  "anya-e1005n:668": "Akodhena jine kodhaṃ,",
  "anya-e1005n:669": "Asādhuṃ sādhunā jine;",
  "anya-e1005n:670": "Jine macchariṃ dānena,",
  "anya-e1005n:671": "Saccenālīkavādinaṃ;",
  "anya-e1005n:672": "130.",
  "anya-e1005n:673": "Adantaṃ damanaṃ dānaṃ,",
  "anya-e1005n:674": "Dānaṃ sabbattha sādhakaṃ;",
  "anya-e1005n:675": "Dānena piya vācāya,",
  "anya-e1005n:676": "Unnamanti namanti ca;",
  "anya-e1005n:677": "131.",
  "anya-e1005n:678": "Dānaṃ sinehabhesajjaṃ,",
  "anya-e1005n:679": "Maccheraṃ dosanosadhaṃ;",
  "anya-e1005n:680": "Dānaṃ yasassī bhesajjaṃ,",
  "anya-e1005n:681": "Maccheraṃ kapaṇosadhaṃ.",
  "anya-e1005n:682": "132.",
  "anya-e1005n:683": "Bahūnamappasārānaṃ,",
  "anya-e1005n:684": "Sāmaggiyā jayaṃ jaye;",
  "anya-e1005n:685": "Tiṇehi vattate yottaṃ,",
  "anya-e1005n:686": "Tena nāgopi bajjhate.",
  "anya-e1005n:687": "133.",
  "anya-e1005n:688": "Sahāyo asamatthopi,",
  "anya-e1005n:689": "Tejasā kiṃkarissati;",
  "anya-e1005n:690": "Nivāte jalito aggi,",
  "anya-e1005n:691": "Sayame vūpasampati.",
  "anya-e1005n:692": "134.",
  "anya-e1005n:693": "Na raññā samakaṃ bhuñje,",
  "anya-e1005n:694": "Kāmabhogaṃ kudācanaṃ;",
  "anya-e1005n:695": "Ākappaṃ rasa bhuttiṃvā,",
  "anya-e1005n:696": "Mālā gandha vilepanaṃ;",
  "anya-e1005n:697": "Vatthaṃ sabbaalaṅkāraṃ,",
  "anya-e1005n:698": "Na raññā sadisaṃ kare.",
  "anya-e1005n:699": "135.",
  "anya-e1005n:700": "Na me rājā sakhā hoti,",
  "anya-e1005n:701": "Na rājā hoti methuno;",
  "anya-e1005n:702": "Eso sāmiko mayhanti,",
  "anya-e1005n:703": "Citte niṭṭhaṃ suthāpaye.",
  "anya-e1005n:704": "136.",
  "anya-e1005n:705": "Nātidūre bhaje rañño,",
  "anya-e1005n:706": "Nāccāsannopavātake;",
  "anya-e1005n:707": "Ujuke nātininne ca,",
  "anya-e1005n:708": "Na bhaje uccamāsane.",
  "anya-e1005n:709": "Chadose vajje sevako,",
  "anya-e1005n:710": "Tiṭṭhe aggiṃva saṃyato.",
  "anya-e1005n:711": "137.",
  "anya-e1005n:712": "Guṇī sabbaññu tulyopi,",
  "anya-e1005n:713": "Nasobhati anissayo;",
  "anya-e1005n:714": "Anagghamopi maṇiseṭṭho,",
  "anya-e1005n:715": "Hemaṃ nissāya sobhati.",
  "anya-e1005n:716": "Rājakaṇḍo niṭṭhito.",
  "anya-e1005n:717": "7. Pakiṇṇakakaṇḍo",
  "anya-e1005n:718": "138.",
  "anya-e1005n:719": "Itthimisse kutosīlaṃ,",
  "anya-e1005n:720": "Maṃsa bhakkhe kutodayā;",
  "anya-e1005n:721": "Surā pāne kutosaccaṃ,",
  "anya-e1005n:722": "Mahālobhe kutohirī;",
  "anya-e1005n:723": "Mahātande kutosippaṃ,",
  "anya-e1005n:724": "Mahā kodhe kutodhanaṃ.",
  "anya-e1005n:725": "139.",
  "anya-e1005n:726": "Surā yogo vikālo ca,",
  "anya-e1005n:727": "Samajja caraṇālasaṃ;",
  "anya-e1005n:728": "Khiḍḍādhutto pāpamitto,",
  "anya-e1005n:729": "Bhoganāsamukhā ime.",
  "anya-e1005n:730": "140.",
  "anya-e1005n:731": "Divā nādikkhā vattabbaṃ,",
  "anya-e1005n:732": "Ratto nāvacanena ca;",
  "anya-e1005n:733": "Sañcareyya bhayā bhīto,",
  "anya-e1005n:734": "Vane vanacarī yathā.",
  "anya-e1005n:735": "141.",
  "anya-e1005n:736": "Jīvantāpi matāpañca,",
  "anya-e1005n:737": "Byāsena parikittitā;",
  "anya-e1005n:738": "Dukkhito byādhitomūḷho,",
  "anya-e1005n:739": "Iṇavā nityasevako.",
  "anya-e1005n:740": "142.",
  "anya-e1005n:741": "Anāgataṃ bhayaṃ disvā,",
  "anya-e1005n:742": "Dūrato parivajjaye;",
  "anya-e1005n:743": "Āgatañca bhayaṃ disvā,",
  "anya-e1005n:744": "A bhīto hoti paṇḍito.",
  "anya-e1005n:745": "143.",
  "anya-e1005n:746": "Niddāluko pamattoca,",
  "anya-e1005n:747": "Sukhatto rogavālaso;",
  "anya-e1005n:748": "Mahiccho kammārāmoca,",
  "anya-e1005n:749": "Satte te satthavajjitā.",
  "anya-e1005n:750": "144.",
  "anya-e1005n:751": "Duggataṃ gaccha he lābha,",
  "anya-e1005n:752": "Lābhī lābhena pūrati;",
  "anya-e1005n:753": "Thale pavassa pajjunna,",
  "anya-e1005n:754": "Sindhu āpena pūrati;",
  "anya-e1005n:755": "Natthidaṃ kammappadhānakaṃ.",
  "anya-e1005n:756": "145.",
  "anya-e1005n:757": "Na hi koci kate kicce,",
  "anya-e1005n:758": "Kattāraṃ samupekkhate;",
  "anya-e1005n:759": "Tasmā sabbāni kiccāni,",
  "anya-e1005n:760": "Sāva sesena kāraye.",
  "anya-e1005n:761": "146.",
  "anya-e1005n:762": "Tūlaṃ sallahukaṃ loke,",
  "anya-e1005n:763": "Tato cāpalla-jātiko;",
  "anya-e1005n:764": "Tato vuḍḍha manovādo,",
  "anya-e1005n:765": "Pamatto buddhasāsane.",
  "anya-e1005n:766": "147.",
  "anya-e1005n:767": "Pāsāṇachattaṃ garukaṃ,",
  "anya-e1005n:768": "Tato devānacikkhaṇaṃ;",
  "anya-e1005n:769": "Tato vuḍḍhānamovādo,",
  "anya-e1005n:770": "Tato buddhassa sāsanaṃ.",
  "anya-e1005n:771": "148.",
  "anya-e1005n:772": "Kāyassa dakkhiṇa hattho,",
  "anya-e1005n:773": "Doso ettha kaniṭṭhako;",
  "anya-e1005n:774": "Kaṇṇa ghānāna-makkhīnaṃ,",
  "anya-e1005n:775": "Vāmo tu pāda-pāsako.",
  "anya-e1005n:776": "149.",
  "anya-e1005n:777": "Tambūlassa majjha patte,",
  "anya-e1005n:778": "Kuvero rakkhatī sadā;",
  "anya-e1005n:779": "Mūlamhi rakkhati yakkho,",
  "anya-e1005n:780": "Aggamhi kālakaṇṇikā;",
  "anya-e1005n:781": "Tāni bhuñjeyya chinditvā,",
  "anya-e1005n:782": "Sirī evaṃ pavaḍḍhati.",
  "anya-e1005n:783": "150.",
  "anya-e1005n:784": "Sampuṇṇarakkho brahmāva,",
  "anya-e1005n:785": "Accurakkho ca bissaṇo;",
  "anya-e1005n:786": "Tasmā hi te pūjayantu,",
  "anya-e1005n:787": "Sadā mānenti taṃ naraṃ.",
  "anya-e1005n:788": "151.",
  "anya-e1005n:789": "Goṇā hi sabbagihīnaṃ,",
  "anya-e1005n:790": "Posakā bhogadāyakā;",
  "anya-e1005n:791": "Tasmā hi mātā pitūva,",
  "anya-e1005n:792": "Mānaye sakkareyya ca.",
  "anya-e1005n:793": "152.",
  "anya-e1005n:794": "Yeca khādanti gomaṃsaṃ,",
  "anya-e1005n:795": "Mātu maṃsaṃva khādare;",
  "anya-e1005n:796": "Matesu tesu gijjhānaṃ,",
  "anya-e1005n:797": "Dade sote ca vāhaye.",
  "anya-e1005n:798": "153.",
  "anya-e1005n:799": "Gurusiddho sippārambho,",
  "anya-e1005n:800": "Ravi sokrā ca majjhimo;",
  "anya-e1005n:801": "Na sippo buddhacandaro,",
  "anya-e1005n:802": "Sorī aṅgāca maraṇaṃ.",
  "anya-e1005n:803": "154.",
  "anya-e1005n:804": "Aṭṭhamiyaṃ guruṃ hanti,",
  "anya-e1005n:805": "Sissaṃ hanti catuddasiṃ;",
  "anya-e1005n:806": "Sippaṃ hanti dasa sippaṃ,",
  "anya-e1005n:807": "Mātāpitā ca puṇṇamiṃ.",
  "anya-e1005n:808": "155.",
  "anya-e1005n:809": "Nāḷikaṃ satta nabhuñje,",
  "anya-e1005n:810": "Na lābuṃ navamaṃ tathā;",
  "anya-e1005n:811": "Dvādasa prinnaṃtriminaṃ,",
  "anya-e1005n:812": "Bhuñje sippaṃ vinassati.",
  "anya-e1005n:813": "156.",
  "anya-e1005n:814": "Ekaṃ caje kulaatthaṃ,",
  "anya-e1005n:815": "Gāmassatthaṃ kuvaṃ caje;",
  "anya-e1005n:816": "Gāma caje janapadatthaṃ,",
  "anya-e1005n:817": "Attatthaṃ pathaviṃ caje.",
  "anya-e1005n:818": "157.",
  "anya-e1005n:819": "Desaṃ ossajja gacchanti,",
  "anya-e1005n:820": "Sīho sappuriso gajo;",
  "anya-e1005n:821": "Tattheva nidhanaṃ yanti,",
  "anya-e1005n:822": "Kāko kāpuriso migo.",
  "anya-e1005n:823": "158.",
  "anya-e1005n:824": "Yamhi padese na māno,",
  "anya-e1005n:825": "Na pemaṃ na ca bandhavā;",
  "anya-e1005n:826": "Na ca vijjāgāho koci,",
  "anya-e1005n:827": "Na tattha vasanaṃ kare.",
  "anya-e1005n:828": "159.",
  "anya-e1005n:829": "Caratyekena pādena,",
  "anya-e1005n:830": "Tiṭṭhatyekena paṇḍito;",
  "anya-e1005n:831": "A nisamma paraṃ ṭhānaṃ,",
  "anya-e1005n:832": "Na pubbamālayaṃ jahe.",
  "anya-e1005n:833": "160.",
  "anya-e1005n:834": "Dhana dhañña payogesu,",
  "anya-e1005n:835": "Tathā vijjāgamesu ca;",
  "anya-e1005n:836": "Dūtesu apacāresu,",
  "anya-e1005n:837": "Cajjā lajjā tadā bhave.",
  "anya-e1005n:838": "161.",
  "anya-e1005n:839": "Dvi guṇo thīnamāhāro,",
  "anya-e1005n:840": "Buddhicāpi catugguṇo;",
  "anya-e1005n:841": "Chagguṇo hoti vāyāmo,",
  "anya-e1005n:842": "Kāmotvaṭṭha-guṇo bhave.",
  "anya-e1005n:843": "162.",
  "anya-e1005n:844": "Pabbe pabbe kamenucchu,",
  "anya-e1005n:845": "Visesarasavāggato;",
  "anya-e1005n:846": "Tathā sumettiko sādhu,",
  "anya-e1005n:847": "Viparītova dujjano.",
  "anya-e1005n:848": "163.",
  "anya-e1005n:849": "Kassako vāṇijo macco,",
  "anya-e1005n:850": "Samaṇo suta sīlavā;",
  "anya-e1005n:851": "Tesu vipula jātesu,",
  "anya-e1005n:852": "Raṭṭhampi vipulaṃ siyā.",
  "anya-e1005n:853": "164.",
  "anya-e1005n:854": "Asajjhāya malā mantā,",
  "anya-e1005n:855": "Anuṭṭhāna malā gharā;",
  "anya-e1005n:856": "Malaṃ vaṇṇassa kosajjaṃ,",
  "anya-e1005n:857": "Pamādo rakkhato malaṃ.",
  "anya-e1005n:858": "165.",
  "anya-e1005n:859": "Hīnānaṃ gacchate vittaṃ,",
  "anya-e1005n:860": "Vīrānaṃ santakattanaṃ;",
  "anya-e1005n:861": "Vadanti ca hīnā janā,",
  "anya-e1005n:862": "Pubba-kammappadhānakā.",
  "anya-e1005n:863": "166.",
  "anya-e1005n:864": "Na vadanti cevaṃdhīrā,",
  "anya-e1005n:865": "Vāyamiṃsu sabbakamme;",
  "anya-e1005n:866": "Na ce sijjhati taṃ kammaṃ,",
  "anya-e1005n:867": "A-phalaṃ eva ko doso.",
  "anya-e1005n:868": "167.",
  "anya-e1005n:869": "Nīcaṃ kulaṃ nipaññaṃ vā,",
  "anya-e1005n:870": "Nirūpaṃ nibalaṃ samaṃ;",
  "anya-e1005n:871": "Imaṃ kālaṃ chuttakālaṃ,",
  "anya-e1005n:872": "Dhanameva visesakaṃ.",
  "anya-e1005n:873": "Pakiṇṇakakaṇḍo niṭṭhito.",
  "anya-e1005n:874": "Paṇḍito sujano kaṇḍo,",
  "anya-e1005n:875": "Dujjano mitta-itthi ca;",
  "anya-e1005n:876": "Rājā pakiṇṇako cāti,",
  "anya-e1005n:877": "Satta-kaṇḍa-vibhūsitaṃ.",
  "anya-e1005n:878": "Visuddhā cāra-therena,",
  "anya-e1005n:879": "Visuddhārāma-vāsinā;",
  "anya-e1005n:880": "Sabba-kulānamatthāya,",
  "anya-e1005n:881": "Visodhitaṃ pathakkhaye."
}