{
  "ja459:0.1": "Jātaka ",
  "ja459:0.2": "Ekādasakanipāta ",
  "ja459:0.3": "Mātuposakavagga ",
  "ja459:0.4": "5. Pānīyajātaka ",
  "ja459:1.1": "“Mitto mittassa pānīyaṁ, ",
  "ja459:1.2": "adinnaṁ paribhuñjisaṁ; ",
  "ja459:1.3": "Tena pacchā vijigucchiṁ, ",
  "ja459:1.4": "taṁ pāpaṁ pakataṁ mayā; ",
  "ja459:1.5": "Mā puna akaraṁ pāpaṁ, ",
  "ja459:1.6": "tasmā pabbajito ahaṁ. ",
  "ja459:2.1": "Paradārañca disvāna, ",
  "ja459:2.2": "chando me udapajjatha; ",
  "ja459:2.3": "Tena pacchā vijigucchiṁ, ",
  "ja459:2.4": "taṁ pāpaṁ pakataṁ mayā; ",
  "ja459:2.5": "Mā puna akaraṁ pāpaṁ, ",
  "ja459:2.6": "tasmā pabbajito ahaṁ. ",
  "ja459:3.1": "Pitaraṁ me mahārāja, ",
  "ja459:3.2": "corā agaṇhu kānane; ",
  "ja459:3.3": "Tesāhaṁ pucchito jānaṁ, ",
  "ja459:3.4": "aññathā naṁ viyākariṁ. ",
  "ja459:4.1": "Tena pacchā vijigucchiṁ, ",
  "ja459:4.2": "taṁ pāpaṁ pakataṁ mayā; ",
  "ja459:4.3": "Mā puna akaraṁ pāpaṁ, ",
  "ja459:4.4": "tasmā pabbajito ahaṁ. ",
  "ja459:5.1": "Pāṇātipātamakaruṁ, ",
  "ja459:5.2": "somayāge upaṭṭhite; ",
  "ja459:5.3": "Tesāhaṁ samanuññāsiṁ. ",
  "ja459:6.1": "Tena pacchā vijigucchiṁ, ",
  "ja459:6.2": "taṁ pāpaṁ pakataṁ mayā; ",
  "ja459:6.3": "Mā puna akaraṁ pāpaṁ, ",
  "ja459:6.4": "tasmā pabbajito ahaṁ. ",
  "ja459:7.1": "Surāmerayamādhukā, ",
  "ja459:7.2": "ye janā paṭhamāsu no; ",
  "ja459:7.3": "Bahūnaṁ te anatthāya, ",
  "ja459:7.4": "majjapānamakappayuṁ; ",
  "ja459:7.5": "Tesāhaṁ samanuññāsiṁ. ",
  "ja459:8.1": "Tena pacchā vijigucchiṁ, ",
  "ja459:8.2": "taṁ pāpaṁ pakataṁ mayā; ",
  "ja459:8.3": "Mā puna akaraṁ pāpaṁ, ",
  "ja459:8.4": "tasmā pabbajito ahaṁ”. ",
  "ja459:9.1": "“Dhiratthu subahū kāme, ",
  "ja459:9.2": "duggandhe bahukaṇṭake; ",
  "ja459:9.3": "Ye ahaṁ paṭisevanto, ",
  "ja459:9.4": "nālabhiṁ tādisaṁ sukhaṁ”. ",
  "ja459:10.1": "“Mahassādā sukhā kāmā, ",
  "ja459:10.2": "Natthi kāmā paraṁ sukhaṁ; ",
  "ja459:10.3": "Ye kāme paṭisevanti, ",
  "ja459:10.4": "Saggaṁ te upapajjare”. ",
  "ja459:11.1": "“Appassādā dukhā kāmā, ",
  "ja459:11.2": "natthi kāmā paraṁ dukhaṁ; ",
  "ja459:11.3": "Ye kāme paṭisevanti, ",
  "ja459:11.4": "nirayaṁ te upapajjare. ",
  "ja459:12.1": "Asī yathā sunisito, ",
  "ja459:12.2": "nettiṁsova supāyiko; ",
  "ja459:12.3": "Sattīva urasi khittā, ",
  "ja459:12.4": "kāmā dukkhatarā tato. ",
  "ja459:13.1": "Aṅgārānaṁva jalitaṁ, ",
  "ja459:13.2": "kāsuṁ sādhikaporisaṁ; ",
  "ja459:13.3": "Phālaṁva divasantattaṁ, ",
  "ja459:13.4": "kāmā dukkhatarā tato. ",
  "ja459:14.1": "Visaṁ yathā halāhalaṁ, ",
  "ja459:14.2": "telaṁ pakkuthitaṁ yathā; ",
  "ja459:14.3": "Tambaloha vilīnaṁva, ",
  "ja459:14.4": "kāmā dukkhatarā tato”ti. ",
  "ja459:15.1": "Pānīyajātakaṁ pañcamaṁ. "
}