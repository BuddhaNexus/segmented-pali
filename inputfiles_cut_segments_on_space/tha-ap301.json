{
  "tha-ap301:0.1": "Therāpadāna ",
  "tha-ap301:0.2": "Citakapūjakavagga ",
  "tha-ap301:0.3": "9. Ambapiṇḍiyattheraapadāna ",
  "tha-ap301:1.1": "“Romaso nāma nāmena, ",
  "tha-ap301:1.2": "dānavo iti vissuto; ",
  "tha-ap301:1.3": "Ambapiṇḍī mayā dinnā, ",
  "tha-ap301:1.4": "vipassissa mahesino. ",
  "tha-ap301:2.1": "Ekanavutito kappe, ",
  "tha-ap301:2.2": "yamambamadadiṁ tadā; ",
  "tha-ap301:2.3": "Duggatiṁ nābhijānāmi, ",
  "tha-ap301:2.4": "ambadānassidaṁ phalaṁ. ",
  "tha-ap301:3.1": "Paṭisambhidā catasso, ",
  "tha-ap301:3.2": "…pe… ",
  "tha-ap301:3.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap301:4.1": "Itthaṁ sudaṁ āyasmā ambapiṇḍiyo thero imā gāthāyo abhāsitthāti. ",
  "tha-ap301:5.1": "Ambapiṇḍiyattherassāpadānaṁ navamaṁ. "
}