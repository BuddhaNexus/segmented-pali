{
  "ja127:0.1": "Jātaka ",
  "ja127:0.2": "Ekakanipāta ",
  "ja127:0.3": "Kusanāḷivagga ",
  "ja127:0.4": "7. Kalaṇḍukajātaka ",
  "ja127:1.1": "“Te desā tāni vatthūni, ",
  "ja127:1.2": "ahañca vanagocaro; ",
  "ja127:1.3": "Anuvicca kho taṁ gaṇheyyuṁ, ",
  "ja127:1.4": "piva khīraṁ kalaṇḍukā”ti. ",
  "ja127:2.1": "Kalaṇḍukajātakaṁ sattamaṁ. "
}