{
  "iti103:0.1": "Itivuttaka 103",
  "iti103:0.2": "Catukkanipāta",
  "iti103:0.3": "Brāhmaṇadhammayāgavagga",
  "iti103:0.4": "Samaṇabrāhmaṇasutta",
  "iti103:1.1": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti103:2.1_0": "“Ye hi keci, bhikkhave, samaṇā vā brāhmaṇā vā ‘idaṁ dukkhan’ti yathābhūtaṁ nappajānanti;",
  "iti103:2.1_1": "‘ayaṁ dukkhasamudayo’ti yathābhūtaṁ nappajānanti; ‘ayaṁ dukkhanirodho’ti yathābhūtaṁ",
  "iti103:2.1_2": "nappajānanti; ‘ayaṁ dukkhanirodhagāminī paṭipadā’ti yathābhūtaṁ nappajānanti—",
  "iti103:2.2_0": "na me te, bhikkhave, samaṇā vā brāhmaṇā vā samaṇesu vā samaṇasammatā brāhmaṇesu vā",
  "iti103:2.2_1": "brāhmaṇasammatā, na ca panete āyasmanto sāmaññatthaṁ vā brahmaññatthaṁ vā diṭṭheva",
  "iti103:2.2_2": "dhamme sayaṁ abhiññā sacchikatvā upasampajja viharanti.",
  "iti103:3.1_0": "Ye ca kho keci, bhikkhave, samaṇā vā brāhmaṇā vā ‘idaṁ dukkhan’ti yathābhūtaṁ pajānanti;",
  "iti103:3.1_1": "‘ayaṁ dukkhasamudayo’ti yathābhūtaṁ pajānanti; ‘ayaṁ dukkhanirodho’ti yathābhūtaṁ",
  "iti103:3.1_2": "pajānanti; ‘ayaṁ dukkhanirodhagāminī paṭipadā’ti yathābhūtaṁ pajānanti—",
  "iti103:3.2_0": "te kho me, bhikkhave, samaṇā vā brāhmaṇā vā samaṇesu ceva samaṇasammatā brāhmaṇesu",
  "iti103:3.2_1": "ca brāhmaṇasammatā, te ca panāyasmanto sāmaññatthañca brahmaññatthañca diṭṭheva dhamme",
  "iti103:3.2_2": "sayaṁ abhiññā sacchikatvā upasampajja viharantī”ti.",
  "iti103:3.3": "Etamatthaṁ bhagavā avoca.",
  "iti103:3.4": "Tatthetaṁ iti vuccati:",
  "iti103:4.1": "“Ye dukkhaṁ nappajānanti,",
  "iti103:4.2": "atho dukkhassa sambhavaṁ;",
  "iti103:4.3": "Yattha ca sabbaso dukkhaṁ,",
  "iti103:4.4": "asesaṁ uparujjhati;",
  "iti103:4.5": "Tañca maggaṁ na jānanti,",
  "iti103:4.6": "dukkhūpasamagāminaṁ.",
  "iti103:5.1": "Cetovimuttihīnā te,",
  "iti103:5.2": "Atho paññāvimuttiyā;",
  "iti103:5.3": "Abhabbā te antakiriyāya,",
  "iti103:5.4": "Te ve jātijarūpagā.",
  "iti103:6.1": "Ye ca dukkhaṁ pajānanti,",
  "iti103:6.2": "atho dukkhassa sambhavaṁ;",
  "iti103:6.3": "Yattha ca sabbaso dukkhaṁ,",
  "iti103:6.4": "asesaṁ uparujjhati;",
  "iti103:6.5": "Tañca maggaṁ pajānanti,",
  "iti103:6.6": "dukkhūpasamagāminaṁ.",
  "iti103:7.1": "Cetovimuttisampannā,",
  "iti103:7.2": "atho paññāvimuttiyā;",
  "iti103:7.3": "Bhabbā te antakiriyāya,",
  "iti103:7.4": "na te jātijarūpagā”ti.",
  "iti103:8.1": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti103:8.2": "Catutthaṁ."
}