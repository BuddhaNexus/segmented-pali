{
  "ja108:0.1": "Jātaka ",
  "ja108:0.2": "Ekakanipāta ",
  "ja108:0.3": "Parosatavagga ",
  "ja108:0.4": "8. Bāhiyajātaka ",
  "ja108:1.1": "“Sikkheyya sikkhitabbāni, ",
  "ja108:1.2": "santi sacchandino janā; ",
  "ja108:1.3": "Bāhiyā hi suhannena, ",
  "ja108:1.4": "rājānamabhirādhayī”ti. ",
  "ja108:2.1": "Bāhiyajātakaṁ aṭṭhamaṁ. "
}