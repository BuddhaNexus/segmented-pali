{
  "anya-e0703n1:0": "Dutiyapariccheda",
  "anya-e0703n1:1": "Mahāsammatavaṁsa",
  "anya-e0703n1:2": "1.",
  "anya-e0703n1:3": " Mahāsammata rājassa, vaṁsajo hi mahāmuni;",
  "anya-e0703n1:4": "Kappassādimhi rājā’si, mahāsammatanāmako.",
  "anya-e0703n1:5": "2.",
  "anya-e0703n1:6": " Rojo ca vararojo ca, tathā kalyāṇakā duve;",
  "anya-e0703n1:7": "Uposatho ca mandhātā, carako’pacarā duve.",
  "anya-e0703n1:8": "3.",
  "anya-e0703n1:9": " Cetīyo mucalo ceva, mahāmūcalanāmako;",
  "anya-e0703n1:10": "Mucalindo sāgaro ceva, sāgaro deva vanāmako.",
  "anya-e0703n1:11": "4.",
  "anya-e0703n1:12": " Bharato bhagīratho ceva, ruci ca suruci pica;",
  "anya-e0703n1:13": "Patāpo mahāpatāpo, panādā ca tathā duve.",
  "anya-e0703n1:14": "5.",
  "anya-e0703n1:15": " Sudassano ca neru ca, tathā eva duve duve;",
  "anya-e0703n1:16": "Pacchimā cā’ti rājāno, tassa puttapaputtakā.",
  "anya-e0703n1:17": "6.",
  "anya-e0703n1:18": " Asaṁkhiyāyukā ete, aṭṭhavīsati bhūmipā;",
  "anya-e0703n1:19": "Kusāvatiṁ rājagahaṁ, mithilañcāpi āvasuṁ.",
  "anya-e0703n1:20": "7.",
  "anya-e0703n1:21": " Tato satañca rājano, chappaññāsa ca saṭṭhi ca;",
  "anya-e0703n1:22": "Caturāsīti sahassāni, chattiṁsā ca tato pare.",
  "anya-e0703n1:23": "8.",
  "anya-e0703n1:24": " Dvattiṁsa aṭṭhavīsā ca, dvāvīsati tato pare;",
  "anya-e0703n1:25": "Aṭṭhārasa sattarasa, pañcadasa catuddasa.",
  "anya-e0703n1:26": "9.",
  "anya-e0703n1:27": " Nava satta dvādasa ca, pañcavīsa tatopare;",
  "anya-e0703n1:28": "Pañcavīsaṁ dvādasa ca, dvādasañca navā pica.",
  "anya-e0703n1:29": "10.",
  "anya-e0703n1:30": " Caturāsītisahassāni, makhādevādikāpi ca;",
  "anya-e0703n1:31": "Caturāsītisahassāni, kaḷārājanakādayo.",
  "anya-e0703n1:32": "11.",
  "anya-e0703n1:33": " Soḷasa yāva okkātaṁ, paputtā rāsito ime;",
  "anya-e0703n1:34": "Visuṁ visuṁ pure rajjaṁ, kamato anusāsisuṁ.",
  "anya-e0703n1:35": "12.",
  "anya-e0703n1:36": " Okkāmukho jeṭṭhaputto, okkākasā’si bhūpati;",
  "anya-e0703n1:37": "Nipuro candīmā candaṁ-mukho ca sivi sañjayo.",
  "anya-e0703n1:38": "13.",
  "anya-e0703n1:39": " Vessantara mahārājā, jālī ca sīhavāhano;",
  "anya-e0703n1:40": "Sīhassaro ca iccete, tassa putta pa puttakā.",
  "anya-e0703n1:41": "14.",
  "anya-e0703n1:42": " Dveasīti sahassāni, sīhassarassa rājino;",
  "anya-e0703n1:43": "Putta pa putta rājāno, jayaseno tadantimo.",
  "anya-e0703n1:44": "15.",
  "anya-e0703n1:45": " Ete kapilavatthusmiṁ, sakyarājāti vissutā;",
  "anya-e0703n1:46": "Sīhahanu mahārājā, jayasenassa atrajo.",
  "anya-e0703n1:47": "16.",
  "anya-e0703n1:48": " Jayasenassa dhītā ca, nāmenā’si yasodharā;",
  "anya-e0703n1:49": "Devadaye devadaha-sakko nāmā’si bhūpati.",
  "anya-e0703n1:50": "17.",
  "anya-e0703n1:51": " Añjano cā’tha kaccānā, āsuṁ tassa sutā duve;",
  "anya-e0703n1:52": "Mahesīcā’si kaccānā, rañño sīhahanussa sā.",
  "anya-e0703n1:53": "18.",
  "anya-e0703n1:54": " Āsī añjanasakkassa, mahesī sā yasodharā;",
  "anya-e0703n1:55": "Añjanassa duve dhītā, māyā cātha pajāpati.",
  "anya-e0703n1:56": "19.",
  "anya-e0703n1:57": " Puttā duve daṇḍapāṇī, suppabuddho ca sākiyo;",
  "anya-e0703n1:58": "Pañca puttā duve dhītā, āsuṁ sīhahanussare.",
  "anya-e0703n1:59": "20.",
  "anya-e0703n1:60": " Suddhodano dhotodano, sakkasukkamitodano;",
  "anya-e0703n1:61": "Amitā pamitācā’ti, ime pañca imā duve.",
  "anya-e0703n1:62": "21.",
  "anya-e0703n1:63": " Suppabuddhassa sakkassa, mahesī amitā ahu;",
  "anya-e0703n1:64": "Tassā’suṁ bhaddakaccānā, devadatto duve sutā.",
  "anya-e0703n1:65": "22.",
  "anya-e0703n1:66": " Māyā mahāpajāpati ceva, suddhodana mahesīyo;",
  "anya-e0703n1:67": "Suddhodana mahārañño, putto māyāya so jino.",
  "anya-e0703n1:68": "23.",
  "anya-e0703n1:69": " Mahā sammatavaṁsamhi, asambhinne mahāmuni;",
  "anya-e0703n1:70": "Evaṁ pavatte sañjāto, sabba khatthiya muddhani.",
  "anya-e0703n1:71": "24.",
  "anya-e0703n1:72": " Siddhatthassa kumārassa, bodhisattassa sā ahu;",
  "anya-e0703n1:73": "Mahesī bhaddakaccānā, putto tassā’si rāhulo.",
  "anya-e0703n1:74": "25.",
  "anya-e0703n1:75": " Bimbisāro ca siddhattha-kumāro ca sahāyakā;",
  "anya-e0703n1:76": "Ubhinnaṁ pitaro cāpi, sahāyāeva te ahuṁ.",
  "anya-e0703n1:77": "26.",
  "anya-e0703n1:78": " Bodhisatto bimbisārā, pañcavassādhiko ahu;",
  "anya-e0703n1:79": "Ekūnatiṁso vayasā, bodhisatto’bhinikkhami.",
  "anya-e0703n1:80": "27.",
  "anya-e0703n1:81": " Padahitvāna chabbassaṁ, bodhiṁ patvā kamena ca;",
  "anya-e0703n1:82": "Pañcatiṁso tha vayasā, bimbisāramupāgami.",
  "anya-e0703n1:83": "28.",
  "anya-e0703n1:84": " Bimbisāro pannarasa-vasso’tha pītaraṁ sayaṁ;",
  "anya-e0703n1:85": "Abhisitto mahāpañño, patto rajjassa tassa tu.",
  "anya-e0703n1:86": "29.",
  "anya-e0703n1:87": " Patte soḷasame vasse, satthā dhammamadesayi;",
  "anya-e0703n1:88": "Dvāpaññāseva vassāni, rajjaṁ kāresi so pana.",
  "anya-e0703n1:89": "30.",
  "anya-e0703n1:90": " Rajje samā pannarasa, pubbe jinasamāgamā;",
  "anya-e0703n1:91": "Sattatiṁsa samā tassa, dharamāne tathāgate.",
  "anya-e0703n1:92": "31.",
  "anya-e0703n1:93": " Bimbisārasuto’jāta-sattutaṁ ghātīyā’mati;",
  "anya-e0703n1:94": "Rajjaṁ dvattiṁsavassāni, mahāmittaddukārayī.",
  "anya-e0703n1:95": "32.",
  "anya-e0703n1:96": " Ajātasattuno vasse, aṭṭhame muni nibbuto;",
  "anya-e0703n1:97": "Pacchā so kārayī rajjaṁ, vassāni catuvīsati.",
  "anya-e0703n1:98": "33.",
  "anya-e0703n1:99": " Tathāgato sakalalokaggataṁ gato;",
  "anya-e0703n1:100": "Aniccatāva samavaso upāgato;",
  "anya-e0703n1:101": "Iti’dha yo bhayajananiṁ aniccataṁ,",
  "anya-e0703n1:102": "Avekkhate sa bhavati dukkhapāragūti.",
  "anya-e0703n1:103": "Sujanappasādasaṁvegatthāya kate mahāvaṁse",
  "anya-e0703n1:104": "Mahāsammatavaṁso nāma",
  "anya-e0703n1:105": "Dutiyo paricchedo."
}