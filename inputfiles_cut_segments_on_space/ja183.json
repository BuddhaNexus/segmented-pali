{
  "ja183:0.1": "Jātaka ",
  "ja183:0.2": "Dukanipāta ",
  "ja183:0.3": "Asadisavagga ",
  "ja183:0.4": "3. Vālodakajātaka ",
  "ja183:1.1": "“Vālodakaṁ apparasaṁ nihīnaṁ, ",
  "ja183:1.2": "Pitvā mado jāyati gadrabhānaṁ; ",
  "ja183:1.3": "Imañca pitvāna rasaṁ paṇītaṁ, ",
  "ja183:1.4": "Mado na sañjāyati sindhavānaṁ”. ",
  "ja183:2.1": "“Appaṁ pivitvāna nihīnajacco, ",
  "ja183:2.2": "So majjatī tena janinda puṭṭho; ",
  "ja183:2.3": "Dhorayhasīlī ca kulamhi jāto, ",
  "ja183:2.4": "Na majjatī aggarasaṁ pivitvā”ti. ",
  "ja183:3.1": "Vālodakajātakaṁ tatiyaṁ. "
}