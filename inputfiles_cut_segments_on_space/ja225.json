{
  "ja225:0.1": "Jātaka ",
  "ja225:0.2": "Dukanipāta ",
  "ja225:0.3": "Kāsāvavagga ",
  "ja225:0.4": "5. Khantivaṇṇajātaka ",
  "ja225:1.1": "“Atthi me puriso deva, ",
  "ja225:1.2": "sabbakiccesu byāvaṭo; ",
  "ja225:1.3": "Tassa cekoparādhatthi, ",
  "ja225:1.4": "tattha tvaṁ kinti maññasi”. ",
  "ja225:2.1": "“Amhākampatthi puriso, ",
  "ja225:2.2": "ediso idha vijjati; ",
  "ja225:2.3": "Dullabho aṅgasampanno, ",
  "ja225:2.4": "khantirasmāka ruccatī”ti. ",
  "ja225:3.1": "Khantivaṇṇajātakaṁ pañcamaṁ. "
}