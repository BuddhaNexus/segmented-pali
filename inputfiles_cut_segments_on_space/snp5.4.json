{
  "snp5.4:0.1": "Sutta Nipāta 5.4 ",
  "snp5.4:0.2": "Puṇṇakamāṇavapucchā ",
  "snp5.4:1.1": "“Anejaṁ mūladassāviṁ, ",
  "snp5.4:1.2": "(iccāyasmā puṇṇako) ",
  "snp5.4:1.3": "Atthi pañhena āgamaṁ; ",
  "snp5.4:1.4": "Kiṁ nissitā isayo manujā, ",
  "snp5.4:1.5": "Khattiyā brāhmaṇā devatānaṁ; ",
  "snp5.4:1.6": "Yaññamakappayiṁsu puthūdha loke, ",
  "snp5.4:1.7": "Pucchāmi taṁ bhagavā brūhi me taṁ”. ",
  "snp5.4:2.1": "“Ye kecime isayo manujā, ",
  "snp5.4:2.2": "(puṇṇakāti bhagavā) ",
  "snp5.4:2.3": "Khattiyā brāhmaṇā devatānaṁ; ",
  "snp5.4:2.4": "Yaññamakappayiṁsu puthūdha loke, ",
  "snp5.4:2.5": "Āsīsamānā puṇṇaka itthattaṁ; ",
  "snp5.4:2.6": "Jaraṁ sitā yaññamakappayiṁsu”. ",
  "snp5.4:3.1": "“Ye kecime isayo manujā, ",
  "snp5.4:3.2": "(iccāyasmā puṇṇako) ",
  "snp5.4:3.3": "Khattiyā brāhmaṇā devatānaṁ; ",
  "snp5.4:3.4": "Yaññamakappayiṁsu puthūdha loke, ",
  "snp5.4:3.5": "Kaccissu te bhagavā yaññapathe appamattā; ",
  "snp5.4:3.6": "Atāruṁ jātiñca jarañca mārisa, ",
  "snp5.4:3.7": "Pucchāmi taṁ bhagavā brūhi me taṁ”. ",
  "snp5.4:4.1": "“Āsīsanti thomayanti abhijappanti juhanti, ",
  "snp5.4:4.2": "(puṇṇakāti bhagavā) ",
  "snp5.4:4.3": "Kāmābhijappanti paṭicca lābhaṁ; ",
  "snp5.4:4.4": "Te yājayogā bhavarāgarattā, ",
  "snp5.4:4.5": "Nātariṁsu jātijaranti brūmi”. ",
  "snp5.4:5.1": "“Te ce nātariṁsu yājayogā, ",
  "snp5.4:5.2": "(iccāyasmā puṇṇako) ",
  "snp5.4:5.3": "Yaññehi jātiñca jarañca mārisa; ",
  "snp5.4:5.4": "Atha ko carahi devamanussaloke, ",
  "snp5.4:5.5": "Atāri jātiñca jarañca mārisa; ",
  "snp5.4:5.6": "Pucchāmi taṁ bhagavā brūhi me taṁ”. ",
  "snp5.4:6.1": "“Saṅkhāya lokasmi paroparāni, ",
  "snp5.4:6.2": "(puṇṇakāti bhagavā) ",
  "snp5.4:6.3": "Yassiñjitaṁ natthi kuhiñci loke; ",
  "snp5.4:6.4": "Santo vidhūmo anīgho nirāso, ",
  "snp5.4:6.5": "Atāri so jātijaranti brūmī”ti. ",
  "snp5.4:6.6": "Puṇṇakamāṇavapucchā tatiyā. "
}