{
  "vv74:0.1": "Vimānavatthu ",
  "vv74:0.2": "Purisavimāna ",
  "vv74:0.3": "Pāyāsivagga ",
  "vv74:0.4": "10. (Uttara) Pāyāsivimānavatthu ",
  "vv74:1.1": "“Yā devarājassa sabhā sudhammā, ",
  "vv74:1.2": "Yatthacchati devasaṅgho samaggo; ",
  "vv74:1.3": "Tathūpamaṁ tuyhamidaṁ vimānaṁ, ",
  "vv74:1.4": "Obhāsayaṁ tiṭṭhati antalikkhe. ",
  "vv74:2.1": "Deviddhipattosi mahānubhāvo, ",
  "vv74:2.2": "Manussabhūto kimakāsi puññaṁ; ",
  "vv74:2.3": "Kenāsi evaṁ jalitānubhāvo, ",
  "vv74:2.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti. ",
  "vv74:3.1": "So devaputto attamano, ",
  "vv74:3.2": "…pe… ",
  "vv74:3.3": "yassa kammassidaṁ phalaṁ. ",
  "vv74:4.1": "“Ahaṁ manussesu manussabhūto, ",
  "vv74:4.2": "Rañño pāyāsissa ahosiṁ māṇavo; ",
  "vv74:4.3": "Laddhā dhanaṁ saṁvibhāgaṁ akāsiṁ, ",
  "vv74:4.4": "Piyā ca me sīlavanto ahesuṁ; ",
  "vv74:4.5": "Annañca pānañca pasannacitto, ",
  "vv74:4.6": "Sakkacca dānaṁ vipulaṁ adāsiṁ. ",
  "vv74:5.1": "Tena metādiso vaṇṇo, ",
  "vv74:5.2": "…pe… ",
  "vv74:5.3": "Vaṇṇo ca me sabbadisā pabhāsatī”ti. ",
  "vv74:6.1": "Pāyāsivimānaṁ dasamaṁ. ",
  "vv74:7.1": "Pāyāsivaggo chaṭṭho. ",
  "vv74:8.1": "Tassuddānaṁ ",
  "vv74:9.1": "Dve agārino phaladāyī, ",
  "vv74:9.2": "Dve upassayadāyī bhikkhāya dāyī; ",
  "vv74:9.3": "Yavapālako ceva dve, ",
  "vv74:9.4": "Kuṇḍalino pāyāsīti. ",
  "vv74:10.1": "Purisānaṁ dutiyo vaggo pavuccatīti. "
}