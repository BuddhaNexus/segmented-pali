{
  "tha-ap127:0.1": "Therāpadāna ",
  "tha-ap127:0.2": "Sereyyavagga ",
  "tha-ap127:0.3": "5. Sammukhāthavikattheraapadāna ",
  "tha-ap127:1.1": "“Jāyamāne vipassimhi, ",
  "tha-ap127:1.2": "nimittaṁ byākariṁ ahaṁ; ",
  "tha-ap127:1.3": "‘Nibbāpayiñca janataṁ, ",
  "tha-ap127:1.4": "buddho loke bhavissati. ",
  "tha-ap127:2.1": "Yasmiñca jāyamānasmiṁ, ",
  "tha-ap127:2.2": "dasasahassi kampati; ",
  "tha-ap127:2.3": "So dāni bhagavā satthā, ",
  "tha-ap127:2.4": "dhammaṁ deseti cakkhumā. ",
  "tha-ap127:3.1": "Yasmiñca jāyamānasmiṁ, ",
  "tha-ap127:3.2": "āloko vipulo ahu; ",
  "tha-ap127:3.3": "So dāni bhagavā satthā, ",
  "tha-ap127:3.4": "dhammaṁ deseti cakkhumā. ",
  "tha-ap127:4.1": "Yasmiñca jāyamānasmiṁ, ",
  "tha-ap127:4.2": "saritāyo na sandayuṁ; ",
  "tha-ap127:4.3": "So dāni bhagavā satthā, ",
  "tha-ap127:4.4": "dhammaṁ deseti cakkhumā. ",
  "tha-ap127:5.1": "Yasmiñca jāyamānasmiṁ, ",
  "tha-ap127:5.2": "avīcaggi na pajjali; ",
  "tha-ap127:5.3": "So dāni bhagavā satthā, ",
  "tha-ap127:5.4": "dhammaṁ deseti cakkhumā. ",
  "tha-ap127:6.1": "Yasmiñca jāyamānasmiṁ, ",
  "tha-ap127:6.2": "pakkhisaṅgho na sañcari; ",
  "tha-ap127:6.3": "So dāni bhagavā satthā, ",
  "tha-ap127:6.4": "dhammaṁ deseti cakkhumā. ",
  "tha-ap127:7.1": "Yasmiñca jāyamānasmiṁ, ",
  "tha-ap127:7.2": "vātakkhandho na vāyati; ",
  "tha-ap127:7.3": "So dāni bhagavā satthā, ",
  "tha-ap127:7.4": "dhammaṁ deseti cakkhumā. ",
  "tha-ap127:8.1": "Yasmiñca jāyamānasmiṁ, ",
  "tha-ap127:8.2": "sabbaratanāni jotayuṁ; ",
  "tha-ap127:8.3": "So dāni bhagavā satthā, ",
  "tha-ap127:8.4": "dhammaṁ deseti cakkhumā. ",
  "tha-ap127:9.1": "Yasmiñca jāyamānasmiṁ, ",
  "tha-ap127:9.2": "sattāsuṁ padavikkamā; ",
  "tha-ap127:9.3": "So dāni bhagavā satthā, ",
  "tha-ap127:9.4": "dhammaṁ deseti cakkhumā. ",
  "tha-ap127:10.1": "Jātamatto ca sambuddho, ",
  "tha-ap127:10.2": "disā sabbā vilokayi; ",
  "tha-ap127:10.3": "Vācāsabhimudīresi, ",
  "tha-ap127:10.4": "esā buddhāna dhammatā’. ",
  "tha-ap127:11.1": "Saṁvejayitvā janataṁ, ",
  "tha-ap127:11.2": "thavitvā lokanāyakaṁ; ",
  "tha-ap127:11.3": "Sambuddhaṁ abhivādetvā, ",
  "tha-ap127:11.4": "pakkāmiṁ pācināmukho. ",
  "tha-ap127:12.1": "Ekanavutito kappe, ",
  "tha-ap127:12.2": "yaṁ buddhamabhithomayiṁ; ",
  "tha-ap127:12.3": "Duggatiṁ nābhijānāmi, ",
  "tha-ap127:12.4": "thomanāya idaṁ phalaṁ. ",
  "tha-ap127:13.1": "Ito navutikappamhi, ",
  "tha-ap127:13.2": "sammukhāthavikavhayo; ",
  "tha-ap127:13.3": "Sattaratanasampanno, ",
  "tha-ap127:13.4": "cakkavattī mahabbalo. ",
  "tha-ap127:14.1": "Pathavīdundubhi nāma, ",
  "tha-ap127:14.2": "ekūnanavutimhito; ",
  "tha-ap127:14.3": "Sattaratanasampanno, ",
  "tha-ap127:14.4": "cakkavattī mahabbalo. ",
  "tha-ap127:15.1": "Aṭṭhāsītimhito kappe, ",
  "tha-ap127:15.2": "obhāso nāma khattiyo; ",
  "tha-ap127:15.3": "Sattaratanasampanno, ",
  "tha-ap127:15.4": "cakkavattī mahabbalo. ",
  "tha-ap127:16.1": "Sattāsītimhito kappe, ",
  "tha-ap127:16.2": "saritacchedanavhayo; ",
  "tha-ap127:16.3": "Sattaratanasampanno, ",
  "tha-ap127:16.4": "cakkavattī mahabbalo. ",
  "tha-ap127:17.1": "Agginibbāpano nāma, ",
  "tha-ap127:17.2": "kappānaṁ chaḷasītiyā; ",
  "tha-ap127:17.3": "Sattaratanasampanno, ",
  "tha-ap127:17.4": "cakkavattī mahabbalo. ",
  "tha-ap127:18.1": "Gatipacchedano nāma, ",
  "tha-ap127:18.2": "kappānaṁ pañcasītiyā; ",
  "tha-ap127:18.3": "Sattaratanasampanno, ",
  "tha-ap127:18.4": "cakkavattī mahabbalo. ",
  "tha-ap127:19.1": "Rājā vātasamo nāma, ",
  "tha-ap127:19.2": "kappānaṁ cullasītiyā; ",
  "tha-ap127:19.3": "Sattaratanasampanno, ",
  "tha-ap127:19.4": "cakkavattī mahabbalo. ",
  "tha-ap127:20.1": "Ratanapajjalo nāma, ",
  "tha-ap127:20.2": "kappānaṁ teasītiyā; ",
  "tha-ap127:20.3": "Sattaratanasampanno, ",
  "tha-ap127:20.4": "cakkavattī mahabbalo. ",
  "tha-ap127:21.1": "Padavikkamano nāma, ",
  "tha-ap127:21.2": "kappānaṁ dveasītiyā; ",
  "tha-ap127:21.3": "Sattaratanasampanno, ",
  "tha-ap127:21.4": "cakkavattī mahabbalo. ",
  "tha-ap127:22.1": "Rājā vilokano nāma, ",
  "tha-ap127:22.2": "kappānaṁ ekasītiyā; ",
  "tha-ap127:22.3": "Sattaratanasampanno, ",
  "tha-ap127:22.4": "cakkavattī mahabbalo. ",
  "tha-ap127:23.1": "Girasāroti nāmena, ",
  "tha-ap127:23.2": "kappesītimhi khattiyo; ",
  "tha-ap127:23.3": "Sattaratanasampanno, ",
  "tha-ap127:23.4": "cakkavattī mahabbalo. ",
  "tha-ap127:24.1": "Paṭisambhidā catasso, ",
  "tha-ap127:24.2": "…pe… ",
  "tha-ap127:24.3": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap127:25.1": "Itthaṁ sudaṁ āyasmā sammukhāthaviko thero imā gāthāyo abhāsitthāti. ",
  "tha-ap127:26.1": "Sammukhāthavikattherassāpadānaṁ pañcamaṁ. "
}