{
  "ja256:0.1": "Jātaka ",
  "ja256:0.2": "Tikanipāta ",
  "ja256:0.3": "Saṅkappavagga ",
  "ja256:0.4": "6. Jarūdapānajātaka ",
  "ja256:1.1": "“Jarūdapānaṁ khaṇamānā, ",
  "ja256:1.2": "vāṇijā udakatthikā; ",
  "ja256:1.3": "Ajjhagamuṁ ayasaṁ lohaṁ, ",
  "ja256:1.4": "tipusīsañca vāṇijā; ",
  "ja256:1.5": "Rajataṁ jātarūpañca, ",
  "ja256:1.6": "muttā veḷuriyā bahū. ",
  "ja256:2.1": "Te ca tena asantuṭṭhā, ",
  "ja256:2.2": "bhiyyo bhiyyo akhāṇisuṁ; ",
  "ja256:2.3": "Te tatthāsīviso ghoro, ",
  "ja256:2.4": "tejassī tejasā hani. ",
  "ja256:3.1": "Tasmā khaṇe nātikhaṇe, ",
  "ja256:3.2": "Atikhātaṁ hi pāpakaṁ; ",
  "ja256:3.3": "Khātena ca dhanaṁ laddhaṁ, ",
  "ja256:3.4": "Atikhātena nāsitan”ti. ",
  "ja256:4.1": "Jarūdapānajātakaṁ chaṭṭhaṁ. "
}