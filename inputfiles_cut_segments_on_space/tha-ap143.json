{
  "tha-ap143:0.1": "Therāpadāna ",
  "tha-ap143:0.2": "Chattavagga ",
  "tha-ap143:0.3": "1 Atichattiyattheraapadāna ",
  "tha-ap143:1.1": "“Parinibbute bhagavati, ",
  "tha-ap143:1.2": "atthadassīnaruttame; ",
  "tha-ap143:1.3": "Chattātichattaṁ kāretvā, ",
  "tha-ap143:1.4": "thūpamhi abhiropayiṁ. ",
  "tha-ap143:2.1": "Kālena kālamāgantvā, ",
  "tha-ap143:2.2": "namassiṁ lokanāyakaṁ; ",
  "tha-ap143:2.3": "Pupphacchadanaṁ katvāna, ",
  "tha-ap143:2.4": "chattamhi abhiropayiṁ. ",
  "tha-ap143:3.1": "Sattarase kappasate, ",
  "tha-ap143:3.2": "devarajjamakārayiṁ; ",
  "tha-ap143:3.3": "Manussattaṁ na gacchāmi, ",
  "tha-ap143:3.4": "thūpapūjāyidaṁ phalaṁ. ",
  "tha-ap143:4.1": "Paṭisambhidā catasso, ",
  "tha-ap143:4.2": "vimokkhāpi ca aṭṭhime; ",
  "tha-ap143:4.3": "Chaḷabhiññā sacchikatā, ",
  "tha-ap143:4.4": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap143:5.1": "Itthaṁ sudaṁ āyasmā atichattiyo thero imā gāthāyo abhāsitthāti. ",
  "tha-ap143:6.1": "Atichattiyattherassāpadānaṁ paṭhamaṁ. "
}