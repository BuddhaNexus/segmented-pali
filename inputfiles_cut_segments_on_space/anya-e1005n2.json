{
  "anya-e1005n2:0": "4. Bāladujjanakaṇḍo",
  "anya-e1005n2:1": "68.",
  "anya-e1005n2:2": "Atippiyo na kātabbo,",
  "anya-e1005n2:3": "Khalo kotūhalaṁ karo;",
  "anya-e1005n2:4": "Sirasā vahyamānopi,",
  "anya-e1005n2:5": "Aḍḍhapūro ghaṭo yathā.",
  "anya-e1005n2:6": "69.",
  "anya-e1005n2:7": "Sappo duṭṭho khalo duṭṭho,",
  "anya-e1005n2:8": "Sappo duṭṭhataro khalo;",
  "anya-e1005n2:9": "Mantosadhehi so sappo,",
  "anya-e1005n2:10": "Khalo kenupasammati.",
  "anya-e1005n2:11": "70.",
  "anya-e1005n2:12": "Yo bālo maññati bālyaṁ,",
  "anya-e1005n2:13": "Paṇḍito vāpi tena so;",
  "anya-e1005n2:14": "Bālova paṇḍitamānī,",
  "anya-e1005n2:15": "So ve bāloti vuccati.",
  "anya-e1005n2:16": "71.",
  "anya-e1005n2:17": "Madhuṁva maññatī bālo,",
  "anya-e1005n2:18": "Yāva pāpaṁ na paccati;",
  "anya-e1005n2:19": "Yadāca paccatī pāpaṁ,",
  "anya-e1005n2:20": "Atha dukkhaṁ nigacchati.",
  "anya-e1005n2:21": "72.",
  "anya-e1005n2:22": "Na sādhu balavā bālo,",
  "anya-e1005n2:23": "Sahasā vindate dhanaṁ;",
  "anya-e1005n2:24": "Kāyassa bhedā duppañño,",
  "anya-e1005n2:25": "Nirayaṁ sopapajjati.",
  "anya-e1005n2:26": "73.",
  "anya-e1005n2:27": "Ghare duṭṭho ca mūsīko ca,",
  "anya-e1005n2:28": "Vane duṭṭho ca vānaro;",
  "anya-e1005n2:29": "Sakuṇe ca duṭṭho kāko,",
  "anya-e1005n2:30": "Nare duṭṭhoca brāhmaṇo.",
  "anya-e1005n2:31": "74.",
  "anya-e1005n2:32": "Dīghā jāgarato ratti,",
  "anya-e1005n2:33": "Dīghaṁ santassa yojanaṁ;",
  "anya-e1005n2:34": "Dīgho bālāna saṁsāro,",
  "anya-e1005n2:35": "Saddhammaṁ a-vijānataṁ.",
  "anya-e1005n2:36": "75.",
  "anya-e1005n2:37": "Tila mattaṁ paresaṁva,",
  "anya-e1005n2:38": "Appa dosañca passati;",
  "anya-e1005n2:39": "Nāḷikerampi sadosaṁ,",
  "anya-e1005n2:40": "Khala-jāto na passati.",
  "anya-e1005n2:41": "76.",
  "anya-e1005n2:42": "Nattadosaṁ pare jaññā,",
  "anya-e1005n2:43": "Jaññā dosaṁ parassatu;",
  "anya-e1005n2:44": "Guyhe kummo aṅgāni,",
  "anya-e1005n2:45": "Para dosañca lakkhaye.",
  "anya-e1005n2:46": "77.",
  "anya-e1005n2:47": "Paṇḍitassa pasaṁsāya,",
  "anya-e1005n2:48": "Daṇḍo bālena dīyate;",
  "anya-e1005n2:49": "Paṇḍito paṇḍiteneva,",
  "anya-e1005n2:50": "Vaṇṇitova suvaṇṇito.",
  "anya-e1005n2:51": "78.",
  "anya-e1005n2:52": "Luddhaṁ atthena gaṇheyya,",
  "anya-e1005n2:53": "Thaddhaṁ añjali kammunā;",
  "anya-e1005n2:54": "Chandānuvattiyā mūḷhaṁ,",
  "anya-e1005n2:55": "Yathābhūtena paṇḍitaṁ.",
  "anya-e1005n2:56": "Bāladujjanakaṇḍo niṭṭhito."
}