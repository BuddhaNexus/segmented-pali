{
  "cp33:0.1": "Cariyāpiṭaka ",
  "cp33:0.2": "Yudhañjayavagga ",
  "cp33:0.3": "Mettāpāramī 1 ",
  "cp33:0.4": "Suvaṇṇasāmacariya ",
  "cp33:1.1": "“Sāmo yadā vane āsiṁ, ",
  "cp33:1.2": "sakkena abhinimmito; ",
  "cp33:1.3": "Pavane sīhabyagghe ca, ",
  "cp33:1.4": "mettāya mupanāmayiṁ. ",
  "cp33:2.1": "Sīhabyagghehi dīpīhi, ",
  "cp33:2.2": "acchehi mahisehi ca; ",
  "cp33:2.3": "Pasadamigavarāhehi, ",
  "cp33:2.4": "parivāretvā vane vasiṁ. ",
  "cp33:3.1": "Na maṁ koci uttasati, ",
  "cp33:3.2": "napi bhāyāmi kassaci; ",
  "cp33:3.3": "Mettābalenupatthaddho, ",
  "cp33:3.4": "ramāmi pavane tadā”ti. ",
  "cp33:4.1": "Suvaṇṇasāmacariyaṁ terasamaṁ. "
}