{
  "ja316:0.1": "Jātaka ",
  "ja316:0.2": "Catukkanipāta ",
  "ja316:0.3": "Pucimandavagga ",
  "ja316:0.4": "6. Sasapaṇḍitajātaka ",
  "ja316:1.1": "“Satta me rohitā macchā, ",
  "ja316:1.2": "udakā thalamubbhatā; ",
  "ja316:1.3": "Idaṁ brāhmaṇa me atthi, ",
  "ja316:1.4": "etaṁ bhutvā vane vasa”. ",
  "ja316:2.1": "“Dussa me khettapālassa, ",
  "ja316:2.2": "rattibhattaṁ apābhataṁ; ",
  "ja316:2.3": "Maṁsasūlā ca dve godhā, ",
  "ja316:2.4": "ekañca dadhivārakaṁ; ",
  "ja316:2.5": "Idaṁ brāhmaṇa me atthi, ",
  "ja316:2.6": "etaṁ bhutvā vane vasa”. ",
  "ja316:3.1": "“Ambapakkaṁ dakaṁ sītaṁ, ",
  "ja316:3.2": "Sītacchāyā manoramā; ",
  "ja316:3.3": "Idaṁ brāhmaṇa me atthi, ",
  "ja316:3.4": "Etaṁ bhutvā vane vasa”. ",
  "ja316:4.1": "“Na sasassa tilā atthi, ",
  "ja316:4.2": "Na muggā napi taṇḍulā; ",
  "ja316:4.3": "Iminā agginā pakkaṁ, ",
  "ja316:4.4": "Mamaṁ bhutvā vane vasā”ti. ",
  "ja316:5.1": "Sasapaṇḍitajātakaṁ chaṭṭhaṁ. "
}