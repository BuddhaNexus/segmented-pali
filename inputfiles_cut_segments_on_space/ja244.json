{
  "ja244:0.1": "Jātaka ",
  "ja244:0.2": "Dukanipāta ",
  "ja244:0.3": "Siṅgālavagga ",
  "ja244:0.4": "4. Vigaticchajātaka ",
  "ja244:1.1": "“Yaṁ passati na taṁ icchati, ",
  "ja244:1.2": "Yañca na passati taṁ kiricchati; ",
  "ja244:1.3": "Maññāmi ciraṁ carissati, ",
  "ja244:1.4": "Na hi taṁ lacchati yaṁ sa icchati. ",
  "ja244:2.1": "Yaṁ labhati na tena tussati, ",
  "ja244:2.2": "Yañca pattheti laddhaṁ hīḷeti; ",
  "ja244:2.3": "Icchā hi anantagocarā, ",
  "ja244:2.4": "Vigaticchāna namo karomase”ti. ",
  "ja244:3.1": "Vigaticchajātakaṁ catutthaṁ. "
}