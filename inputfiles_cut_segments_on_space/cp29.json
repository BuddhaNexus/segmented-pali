{
  "cp29:0.1": "Cariyāpiṭaka ",
  "cp29:0.2": "Yudhañjayavagga ",
  "cp29:0.3": "Saccapāramī 3 ",
  "cp29:0.4": "Vaṭṭapotakacariya ",
  "cp29:1.1": "“Punāparaṁ yadā homi, ",
  "cp29:1.2": "magadhe vaṭṭapotako; ",
  "cp29:1.3": "Ajātapakkho taruṇo, ",
  "cp29:1.4": "maṁsapesi kulāvake. ",
  "cp29:2.1": "Mukhatuṇḍakenāharitvā, ",
  "cp29:2.2": "mātā posayatī mamaṁ; ",
  "cp29:2.3": "Tassā phassena jīvāmi, ",
  "cp29:2.4": "natthi me kāyikaṁ balaṁ. ",
  "cp29:3.1": "Saṁvacchare gimhasamaye, ",
  "cp29:3.2": "davaḍāho padippati; ",
  "cp29:3.3": "Upagacchati amhākaṁ, ",
  "cp29:3.4": "pāvako kaṇhavattanī. ",
  "cp29:4.1": "Dhamadhamāiti evaṁ, ",
  "cp29:4.2": "saddāyanto mahāsikhī; ",
  "cp29:4.3": "Anupubbena jhāpento, ",
  "cp29:4.4": "aggi mamamupāgami. ",
  "cp29:5.1": "Aggivegabhayātītā, ",
  "cp29:5.2": "tasitā mātāpitā mama; ",
  "cp29:5.3": "Kulāvake maṁ chaḍḍetvā, ",
  "cp29:5.4": "attānaṁ parimocayuṁ. ",
  "cp29:6.1": "Pāde pakkhe pajahāmi, ",
  "cp29:6.2": "natthi me kāyikaṁ balaṁ; ",
  "cp29:6.3": "Sohaṁ agatiko tattha, ",
  "cp29:6.4": "evaṁ cintesahaṁ tadā. ",
  "cp29:7.1": "‘Yesāhaṁ upadhāveyyaṁ, ",
  "cp29:7.2": "bhīto tasitavedhito; ",
  "cp29:7.3": "Te maṁ ohāya pakkantā, ",
  "cp29:7.4": "kathaṁ me ajja kātave. ",
  "cp29:8.1": "Atthi loke sīlaguṇo, ",
  "cp29:8.2": "saccaṁ soceyyanuddayā; ",
  "cp29:8.3": "Tena saccena kāhāmi, ",
  "cp29:8.4": "saccakiriyamuttamaṁ. ",
  "cp29:9.1": "Āvejjetvā dhammabalaṁ, ",
  "cp29:9.2": "saritvā pubbake jine; ",
  "cp29:9.3": "Saccabalamavassāya, ",
  "cp29:9.4": "saccakiriyamakāsahaṁ. ",
  "cp29:10.1": "Santi pakkhā apatanā, ",
  "cp29:10.2": "santi pādā avañcanā; ",
  "cp29:10.3": "Mātāpitā ca nikkhantā, ",
  "cp29:10.4": "jātaveda paṭikkama’. ",
  "cp29:11.1": "Sahasacce kate mayhaṁ, ",
  "cp29:11.2": "Mahāpajjalito sikhī; ",
  "cp29:11.3": "Vajjesi soḷasakarīsāni, ",
  "cp29:11.4": "Udakaṁ patvā yathā sikhī; ",
  "cp29:11.5": "Saccena me samo natthi, ",
  "cp29:11.6": "Esā me saccapāramī”ti. ",
  "cp29:12.1": "Vaṭṭapotakacariyaṁ navamaṁ. "
}