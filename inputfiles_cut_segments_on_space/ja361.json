{
  "ja361:0.1": "Jātaka ",
  "ja361:0.2": "Pañcakanipāta ",
  "ja361:0.3": "Vaṇṇārohavagga ",
  "ja361:0.4": "1. Vaṇṇārohajātaka ",
  "ja361:1.1": "“Vaṇṇārohena jātiyā, ",
  "ja361:1.2": "balanikkamanena ca; ",
  "ja361:1.3": "Subāhu na mayā seyyo, ",
  "ja361:1.4": "sudāṭha iti bhāsasi”. ",
  "ja361:2.1": "“Vaṇṇārohena jātiyā, ",
  "ja361:2.2": "balanikkamanena ca; ",
  "ja361:2.3": "Sudāṭho na mayā seyyo, ",
  "ja361:2.4": "subāhu iti bhāsasi. ",
  "ja361:3.1": "Evañce maṁ viharantaṁ, ",
  "ja361:3.2": "subāhu samma dubbhasi; ",
  "ja361:3.3": "Na dānāhaṁ tayā saddhiṁ, ",
  "ja361:3.4": "saṁvāsamabhirocaye. ",
  "ja361:4.1": "Yo paresaṁ vacanāni, ",
  "ja361:4.2": "saddaheyya yathātathaṁ; ",
  "ja361:4.3": "Khippaṁ bhijjetha mittasmiṁ, ",
  "ja361:4.4": "verañca pasave bahuṁ. ",
  "ja361:5.1": "Na so mitto yo sadā appamatto, ",
  "ja361:5.2": "Bhedāsaṅkī randhamevānupassī; ",
  "ja361:5.3": "Yasmiñca setī urasīva putto, ",
  "ja361:5.4": "Sa ve mitto yo abhejjo parehī”ti. ",
  "ja361:6.1": "Vaṇṇārohajātakaṁ paṭhamaṁ. "
}