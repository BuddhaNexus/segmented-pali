{
  "ja38:0.1": "Jātaka ",
  "ja38:0.2": "Ekakanipāta ",
  "ja38:0.3": "Kulāvakavagga ",
  "ja38:0.4": "8. Bakajātaka ",
  "ja38:1.1": "“Nāccantaṁ nikatippañño, ",
  "ja38:1.2": "nikatyā sukhamedhati; ",
  "ja38:1.3": "Ārādheti nikatippañño, ",
  "ja38:1.4": "bako kakkaṭakāmivā”ti. ",
  "ja38:2.1": "Bakajātakaṁ aṭṭhamaṁ. "
}