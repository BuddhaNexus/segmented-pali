{
  "anya-e0606n:0": " Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e0606n:1": "Kamalāñjali",
  "anya-e0606n:2": "1.",
  "anya-e0606n:3": " Kamalāsana kamalāpati pamathādhipa vajirā,",
  "anya-e0606n:4": "Yudhadānava manujoraga bhujagāsana patinaṁ;",
  "anya-e0606n:5": "Makuṭāhita manidīdhiti bhamarāvali bhajitaṁ,",
  "anya-e0606n:6": "Paṇamā’mahaṁ anaghaṁ muni caraṇāmala kamalaṁ.",
  "anya-e0606n:7": "2.",
  "anya-e0606n:8": " Kamalālaya sadi’saṅkita kalamaṅgala ruciraṁ,",
  "anya-e0606n:9": "Jutirañjitaṁ abhinijjita subhakañcana nalinaṁ;",
  "anya-e0606n:10": "Janitā cira samaye nija pitu bhūpati namitaṁ,",
  "anya-e0606n:11": "Paṇamā’mahaṁ anaghaṁ muni caraṇāmala kamalaṁ.",
  "anya-e0606n:12": "3.",
  "anya-e0606n:13": " Saraṇāgata rajanīpati dinasekhara namitaṁ,",
  "anya-e0606n:14": "Bhajitākhila janapāvanaṁ abhikaṅkhita sukhadaṁ;",
  "anya-e0606n:15": "Vasudhātala sayamuggata sarasīruha mahitaṁ,",
  "anya-e0606n:16": "Paṇamā’mahaṁ anaghaṁ muni caraṇāmala kamalaṁ.",
  "anya-e0606n:17": "4.",
  "anya-e0606n:18": " Diradāsana tarupantika ghaṭitāsanaṁ abhayaṁ,",
  "anya-e0606n:19": "Vijayāsana samadhiṭṭhita caturaṅgika viriyaṁ;",
  "anya-e0606n:20": "Dharamānaka suriyehani vijitantaka dhajiniṁ,",
  "anya-e0606n:21": "Paṇamā’mahaṁ abhipātita namuciddhaja vibhavaṁ.",
  "anya-e0606n:22": "5.",
  "anya-e0606n:23": " Sadayodita piyabhārati vijitantaka samaraṁ,",
  "anya-e0606n:24": "Dasapārami balakampita sadharādhara dharaniṁ;",
  "anya-e0606n:25": "Girimekhala varavārana sirasānata caraṇaṁ,",
  "anya-e0606n:26": "Paṇamā’mahaṁ abhinandita sanarāmara bhuvanaṁ.",
  "anya-e0606n:27": "6.",
  "anya-e0606n:28": " Nikhilāsava vigame’nativimalīkata hadayaṁ,",
  "anya-e0606n:29": "Tadanantara viditākhila matigocara visayaṁ;",
  "anya-e0606n:30": "Visayīkata bhuvanattayaṁ atilokiya caritaṁ,",
  "anya-e0606n:31": "Paṇamā’mahaṁ aparājitaṁ arahaṁ muniṁ asamaṁ.",
  "anya-e0606n:32": "7.",
  "anya-e0606n:33": " Mudubhārati madhupāsita nalinopama vadanaṁ,",
  "anya-e0606n:34": "Rucirāyata nalinīdala nibha locana yugalaṁ;",
  "anya-e0606n:35": "Udayodita ravimaṇḍala jalitāmala niṭilaṁ,",
  "anya-e0606n:36": "Paṇamā’maha akutobhayaṁ anaghaṁ muni pamukhaṁ.",
  "anya-e0606n:37": "8.",
  "anya-e0606n:38": " Asitambuda rucikuñcita mudu kuntala lalitaṁ,",
  "anya-e0606n:39": "Bhuvanodara vitatāmita jutisañcaya jalitaṁ;",
  "anya-e0606n:40": "Madamodita diradopama gativibbhama ruciraṁ,",
  "anya-e0606n:41": "Paṇamā’mahaṁ amatandada munipuṅgavaṁ asamaṁ.",
  "anya-e0606n:42": "9.",
  "anya-e0606n:43": " Karuṇārasa paribhāvita savaṇāmata vacanaṁ,",
  "anya-e0606n:44": "Virudāvali sataghosita yasapūrita bhuvanaṁ;",
  "anya-e0606n:45": "Sumanohara varalakkhaṇa sirisañcaya sadanaṁ,",
  "anya-e0606n:46": "Paṇamā’mahaṁ uditāmala sasimaṇḍala vadanaṁ.",
  "anya-e0606n:47": "10.",
  "anya-e0606n:48": " Vinayāraha janamānasa kumudākara sasinaṁ,",
  "anya-e0606n:49": "Tasināpaga parisosana satadīdhiti tulitaṁ;",
  "anya-e0606n:50": "Tamanāsava munisevitaṁ apalokita sukhadaṁ,",
  "anya-e0606n:51": "Paṇamā’mahaṁ aniketanaṁ akhilāgati vigataṁ.",
  "anya-e0606n:52": "11.",
  "anya-e0606n:53": " Sahitākhila bhayabheravaṁ abhayāgata saranaṁ,",
  "anya-e0606n:54": "Ajarāmara sukhadāyakaṁ anirākata karuṇaṁ;",
  "anya-e0606n:55": "Tamupāsaka janasevita supatiṭṭhita caraṇaṁ,",
  "anya-e0606n:56": "Paṇamā’mahaṁ ahitāpahaṁ anaghuttama caraṇaṁ.",
  "anya-e0606n:57": "12.",
  "anya-e0606n:58": " Karuṇāmata rasapurita vīmalākhila hadayaṁ,",
  "anya-e0606n:59": "Vihitāmita janatāhitaṁ anukampita bhuvanaṁ;",
  "anya-e0606n:60": "Bhuvane sutaṁ avanīpati sata sevita caraṇaṁ,",
  "anya-e0606n:61": "Paṇamā’mahaṁ anaghaṁ muniṁ aghanāsana caturaṁ.",
  "anya-e0606n:62": "13.",
  "anya-e0606n:63": " Aratīrati paripīlita yatimānasa damanaṁ,",
  "anya-e0606n:64": "Nijasāsana vinivārita puthutitthiya samaṇaṁ;",
  "anya-e0606n:65": "Paravādika janatākata paribhāsita khamanaṁ,",
  "anya-e0606n:66": "Paṇamā’mahaṁ atidevata vara gotama samaṇaṁ.",
  "anya-e0606n:67": "14.",
  "anya-e0606n:68": " Saraṇāgata bhayanāsana vajirālaya paṇibhaṁ,",
  "anya-e0606n:69": "Bhavasāgara patitāmita janatārana nirataṁ;",
  "anya-e0606n:70": "Sirasāvahaṁ amalañjali puṭapaṅkaja makulaṁ,",
  "anya-e0606n:71": "Paṇamā’mahaṁ akhilalāya vigataṁ muniṁ atulaṁ.",
  "anya-e0606n:72": "15.",
  "anya-e0606n:73": " Vimalīkata janamānasa vigatāsava bhagavaṁ,",
  "anya-e0606n:74": "Bhavapāraga vibhavāmata sukhadāyaka satataṁ;",
  "anya-e0606n:75": "Paramādara garugārava vinataṁ jina payataṁ,",
  "anya-e0606n:76": "Padapaṅkaja rajasā mama samalaṅkuru sirasaṁ.",
  "anya-e0606n:77": "16.",
  "anya-e0606n:78": " Pavanāhata dumapallavaṁ iva nārata capalaṁ,",
  "anya-e0606n:79": "Bhavalālasa malinīkataṁ ajitindriya nivahaṁ;",
  "anya-e0606n:80": "Cira sañcita duritāhataṁ anivārita timisaṁ,",
  "anya-e0606n:81": "Vimalīkuru karuṇābhara sutaraṁ mama hadayaṁ.",
  "anya-e0606n:82": "17.",
  "anya-e0606n:83": " Adaye dayaṁ anaye nayaṁ api yo guṇaṁ aguṇe,",
  "anya-e0606n:84": "Ahite hitaṁ akaro kvacida api kenaci nakataṁ;",
  "anya-e0606n:85": "Sadaye jina sunaye guṇasadane tayi nitaraṁ,",
  "anya-e0606n:86": "Suhite hitacarite’nagha ramate mama hadayaṁ.",
  "anya-e0606n:87": "18.",
  "anya-e0606n:88": " Bhavasaṅkaṭa patitenapi bhavatā cira caritaṁ,",
  "anya-e0606n:89": "Visame sama caraṇaṁ khalu dasapārami bharaṇaṁ;",
  "anya-e0606n:90": "Sarato’hani sarato nisi supinenapi satataṁ,",
  "anya-e0606n:91": "Ramate jina sumate tvayi sadayaṁ mama hadayaṁ.",
  "anya-e0606n:92": "19.",
  "anya-e0606n:93": " Atiduddadaṁ adadī bhavaṁ atidukkaraṁ akarī;",
  "anya-e0606n:94": "Atidukkhamaṁ akhamī vata karuṇānidhi’rasamo,",
  "anya-e0606n:95": "Iti te guṇaṁ anaghaṁ muni sarato mama hadayaṁ,",
  "anya-e0606n:96": "Ramate’hani ramate nisi ramate tvayi satataṁ.",
  "anya-e0606n:97": "20.",
  "anya-e0606n:98": " Atiduccaraṁ acarī bhavaṁ atiduddamaṁ adamī,",
  "anya-e0606n:99": "Atiduddayaṁ adayī vata sadayāpara hadayo;",
  "anya-e0606n:100": "Iti te guṇaṁ anaghaṁ muni sarato mama hadayaṁ,",
  "anya-e0606n:101": "Ramate’hani ramate nisi ramate tvayi satataṁ.",
  "anya-e0606n:102": "21.",
  "anya-e0606n:103": " Atiduggamaṁ agamī bhavaṁ atidujjayaṁ ajayī,",
  "anya-e0606n:104": "Atidussahaṁ asahī vata samupekkhita manaso;",
  "anya-e0606n:105": "Iti te guṇaṁ anaghaṁ muni sarato mama hadayaṁ,",
  "anya-e0606n:106": "Ramate’hani ramate nisi ramate tvayi satataṁ.",
  "anya-e0606n:107": "22.",
  "anya-e0606n:108": " Atidāruna palayānala sadisānala jalite,",
  "anya-e0606n:109": "Niraye vinipatito ciraṁ aghatāpita manaso;",
  "anya-e0606n:110": "Na sariṁ sakida api te pita bhajituṁ padanalinaṁ,",
  "anya-e0606n:111": "Khama gotama narasārathi tamidaṁ mama khalitaṁ.",
  "anya-e0606n:112": "23.",
  "anya-e0606n:113": " Tiriyaggata-gatiyaṁ ciraṁ anavaṭṭhita carito,",
  "anya-e0606n:114": "Atiniṭṭhura vadhatajjita bhayakampita hadayo;",
  "anya-e0606n:115": "Na sariṁ sakidapi te pita bhajituṁ padanalinaṁ,",
  "anya-e0606n:116": "Khama gotama purisuttama tamidaṁ mama khalitaṁ.",
  "anya-e0606n:117": "24.",
  "anya-e0606n:118": " Paridevana nirato ciraṁ atha pettiya visaye,",
  "anya-e0606n:119": "Sujighacchita supipāsita parisosita jaṭhare;",
  "anya-e0606n:120": "Na sariṁ sakidapi te pita bhajituṁ padanalinaṁ,",
  "anya-e0606n:121": "Khama gotama dipaduttama tamidaṁ mama khalitaṁ.",
  "anya-e0606n:122": "25.",
  "anya-e0606n:123": " Vivaso bhusaṁ aghadūsita manasāsura visaye,",
  "anya-e0606n:124": "Janito ghanatimire ciraṁ atidukkhita hadayo;",
  "anya-e0606n:125": "Na sariṁ sakidapi te pita bhajituṁ padanalinaṁ,",
  "anya-e0606n:126": "Khama gotama vasisattama tamidaṁ mama khalitaṁ.",
  "anya-e0606n:127": "26.",
  "anya-e0606n:128": " Manasā cira vihitaṁ saraṁ atikibbisa caritaṁ,",
  "anya-e0606n:129": "Samathenatha suvirājiya taṁ asaññitaṁ upago;",
  "anya-e0606n:130": "Na sariṁ sakida api te pita bhajituṁ pada nalinaṁ,",
  "anya-e0606n:131": "Khama gotama vasipuṅgava tamidaṁ mama khalitaṁ.",
  "anya-e0606n:132": "27.",
  "anya-e0606n:133": " Vijigucchiya duritaṁ nija vapusā kataṁ amitaṁ,",
  "anya-e0606n:134": "Tanu vajjitaṁ upago bhavaṁ iha bhāvita samatho;",
  "anya-e0606n:135": "Na sariṁ sakidapi te pita bhajituṁ padanalinaṁ,",
  "anya-e0606n:136": "Khama gotama yatikuñjara tamidaṁ mama khalitaṁ.",
  "anya-e0606n:137": "28.",
  "anya-e0606n:138": " Ratanattaya rahite bhusa bahulīkata durite,",
  "anya-e0606n:139": "Janito paravisaye budhajana nindiya carite;",
  "anya-e0606n:140": "Na sariṁ sakidapi te pita bhajituṁ padanalinaṁ,",
  "anya-e0606n:141": "Khama gotama karuṇānidhi tamidaṁ mama khalitaṁ.",
  "anya-e0606n:142": "29.",
  "anya-e0606n:143": " Janito yadi manujesupi vikalindriya nivaho,",
  "anya-e0606n:144": "Tanunā karacaraṇādihi vikate’niha dukhito;",
  "anya-e0606n:145": "Na sariṁ sakidapi te pita bhajituṁ padanalinaṁ,",
  "anya-e0606n:146": "Khama gotama matisāgara tamidaṁ mama khalitaṁ.",
  "anya-e0606n:147": "30.",
  "anya-e0606n:148": " Vidhināhita matibhāvana rahito tamapihito,",
  "anya-e0606n:149": "Visadesupi kusalādisu tathadassana vimukho;",
  "anya-e0606n:150": "Na sariṁ sakidapi te pita bhajituṁ padanalinaṁ,",
  "anya-e0606n:151": "Khama gotama narakesari tamidaṁ mama khalitaṁ.",
  "anya-e0606n:152": "31.",
  "anya-e0606n:153": " Sucirenapi bhuvi dullabhaṁ asamaṁ khaṇaṁ alabhaṁ,",
  "anya-e0606n:154": "Sanarāmara janatāhita sukhadaṁ muni jananaṁ;",
  "anya-e0606n:155": "Na sariṁ sakidapi te pita bhajituṁ padanalinaṁ,",
  "anya-e0606n:156": "Khama gotama vadataṁvara tamidaṁ mama khalitaṁ.",
  "anya-e0606n:157": "32.",
  "anya-e0606n:158": " Nisitāyudha vadhasajjita khaḷaniddaya hadayo,",
  "anya-e0606n:159": "Parahiṁsana ruci bhiṁsana yamasodara sadiso;",
  "anya-e0606n:160": "Na sariṁ sakidapi te pita bhajituṁ padanalinaṁ,",
  "anya-e0606n:161": "Khama gotama munisattama tamidaṁ mama khalitaṁ.",
  "anya-e0606n:162": "33.",
  "anya-e0606n:163": " Parasantaka haraṇe katamati bañcana bahulo,",
  "anya-e0606n:164": "Gharasandhika paripanthika sahasākati nirato;",
  "anya-e0606n:165": "Na sariṁ sakidapi te pita bhajituṁ padanalinaṁ,",
  "anya-e0606n:166": "Khama gotama munipuṅgava tamidaṁ mama khalitaṁ.",
  "anya-e0606n:167": "34.",
  "anya-e0606n:168": " Navayobbana madagabbita parimucchita hadayo,",
  "anya-e0606n:169": "Sucisajjana vijigucchiya paradārika nirato;",
  "anya-e0606n:170": "Na sariṁ sakidapi te pita bhajituṁ padanalinaṁ,",
  "anya-e0606n:171": "Khama gotama karuṇābhara tamidaṁ mama khalitaṁ.",
  "anya-e0606n:172": "35.",
  "anya-e0606n:173": " Madirāsavarata nāgarajana santata bhajito,",
  "anya-e0606n:174": "Garugārava hiridūrita tiriyaggata carito;",
  "anya-e0606n:175": "Na sariṁ sakidapi te pita bhajituṁ padanalinaṁ,",
  "anya-e0606n:176": "Khama gotama vihatāsava tamidaṁ mama khalitaṁ.",
  "anya-e0606n:177": "36.",
  "anya-e0606n:178": " Sapitāmaha papitāmaha nicitaṁ dhanaṁ amitaṁ,",
  "anya-e0606n:179": "Pitusañcitamapi nāsiya kitavo hatavibhavo;",
  "anya-e0606n:180": "Na sariṁ sakidapi te pita bhajituṁ padanalinaṁ,",
  "anya-e0606n:181": "Khama gotama vijitantaka tamidaṁ mama khalitaṁ.",
  "anya-e0606n:182": "37.",
  "anya-e0606n:183": " Vitathālika vacano parapiyasuññata karaṇo,",
  "anya-e0606n:184": "Pharusaṁ bhaṇaṁ atinipphala bahubhāsana nipuṇo;",
  "anya-e0606n:185": "Na sariṁ sakidapi te pita bhajituṁ padanalinaṁ,",
  "anya-e0606n:186": "Khama gotama bhuvanesuta tamidaṁ mama khalitaṁ.",
  "anya-e0606n:187": "38.",
  "anya-e0606n:188": " Parasampadaṁ abhijhāyana nirantara dukhito,",
  "anya-e0606n:189": "Nabhirajjhana paramo kvaci phaladassana rahito;",
  "anya-e0606n:190": "Na sariṁ sakidapi te pita bhajituṁ padanalinaṁ,",
  "anya-e0606n:191": "Khama gotama guṇasāgara tamidaṁ mama khalitaṁ.",
  "anya-e0606n:192": "39.",
  "anya-e0606n:193": " Bhavato bhavaṁ aparāparaṁ ayatāciraṁ iti me,",
  "anya-e0606n:194": "Vapusā atha vacasāpi ca manasā kataṁ amitaṁ;",
  "anya-e0606n:195": "Khama gotama duritāpaha duritaṁ bahuvihitaṁ,",
  "anya-e0606n:196": "Dada me sivapadaṁ accutaṁ amataṁ bhavavigataṁ.",
  "anya-e0606n:197": "40.",
  "anya-e0606n:198": " Timirāvuta kuṇapākula vijigucchiya pavane,",
  "anya-e0606n:199": "Janikāsuci jaṭhare bahu kimisantati sadane;",
  "anya-e0606n:200": "Asayiṁ bhavagahaṇe caraṁ ahaṁ appaṭisaraṇe,",
  "anya-e0606n:201": "Saranaṁ bhava bhagavaṁ mama bhavanīradhi taraṇe.",
  "anya-e0606n:202": "41.",
  "anya-e0606n:203": " Bahi nikkhamaṁ asakiṁ bhagatiriyaṁ patha patito,",
  "anya-e0606n:204": "Agadaṅkara kata sallaka satakhaṇdita karaṇo;",
  "anya-e0606n:205": "Amariṁ bhavagahaṇe caraṁ ahaṁ appaṭisaraṇe,",
  "anya-e0606n:206": "Saraṇaṁ bhava bhagavaṁ mama bhavanīradhi tarane.",
  "anya-e0606n:207": "42.",
  "anya-e0606n:208": " Patito bahi ratipillaka tanurāmaya mathito,",
  "anya-e0606n:209": "Vadituṁ kimu viditumpi ca na sahaṁ matirahito;",
  "anya-e0606n:210": "Amariṁ bhavagahaṇe caraṁ ahaṁ apphṭisaraṇe,",
  "anya-e0606n:211": "Saraṇaṁ bhava bhagavaṁ mama bhavanīradhi taraṇe.",
  "anya-e0606n:212": "43.",
  "anya-e0606n:213": " Janito yadi sukhito janadayito piyajanako,",
  "anya-e0606n:214": "Puthuko bahuvidha-kīlananirato gadagahito;",
  "anya-e0606n:215": "Amariṁ bhavagahaṇe caraṁ ahaṁ appaṭisaraṇe,",
  "anya-e0606n:216": "Saraṇaṁ bhava bhagavaṁ mama bhavanīradhi taraṇe.",
  "anya-e0606n:217": "44.",
  "anya-e0606n:218": " Tarunopi hi gharabandhana gathitomita vibhavo,",
  "anya-e0606n:219": "Sahasā gadagahito piyabhariyāsuta viyuto;",
  "anya-e0606n:220": "Amariṁ bhavagahaṇe caraṁ ahaṁ appaṭisaraṇe,",
  "anya-e0606n:221": "Saraṇaṁ bhava bhagavaṁ mama bhavanīradhi taraṇe.",
  "anya-e0606n:222": "45.",
  "anya-e0606n:223": " Ajaraṁ tanuṁ abhimaññiya navayobbana vasiko,",
  "anya-e0606n:224": "Jarasā parimathito paraṁ ananuṭṭhita kusalo;",
  "anya-e0606n:225": "Amariṁ bhavagahaṇe caraṁ ahaṁ appaṭisaraṇe,",
  "anya-e0606n:226": "Saraṇaṁ bhava bhagavaṁ mama bhavanīradhi taraṇe.",
  "anya-e0606n:227": "46.",
  "anya-e0606n:228": " Nirujo dhuvaṁ arujaṁ tanuṁ abhimaññiya samado,",
  "anya-e0606n:229": "Kusalāsaya vimukho bhusaṁ avaso gadanihato;",
  "anya-e0606n:230": "Amariṁ bhavagahaṇe caraṁ ahaṁ appaṭisaraṇe,",
  "anya-e0606n:231": "Saraṇaṁ bhava bhagavaṁ mama bhavanīradhi taraṇe.",
  "anya-e0606n:232": "47.",
  "anya-e0606n:233": " Pavicintiya sakajīvitaṁ amaraṁ dhuvaṁ anighaṁ,",
  "anya-e0606n:234": "Iti jīvitamadagabbita matirujjhita kusalo;",
  "anya-e0606n:235": "Amariṁ bhavagahaṇe caraṁ ahaṁ appaṭisaraṇe,",
  "anya-e0606n:236": "Saraṇaṁ bhava bhagavaṁ mama bhavanīradhi taraṇe.",
  "anya-e0606n:237": "48.",
  "anya-e0606n:238": " Parahiṁsana dhanamosana paradārika nirato,",
  "anya-e0606n:239": "Dharanīpati gahito bahu vadhabandhana nihato;",
  "anya-e0606n:240": "Amariṁ bhavagahaṇe caraṁ ahaṁ appaṭisaraṇe,",
  "anya-e0606n:241": "Saraṇaṁ bhava bhagavaṁ mama bhavanīradhi taraṇe.",
  "anya-e0606n:242": "49.",
  "anya-e0606n:243": " Narakodaka patito giritarumatthaka galito,",
  "anya-e0606n:244": "Migavāḷaka gahito visadhara jātihi ḍasito;",
  "anya-e0606n:245": "Amariṁ bhavagahaṇe caraṁ ahaṁ appaṭisaraṇe,",
  "anya-e0606n:246": "Saraṇaṁ bhava bhagavaṁ mama bhavanīradhi taraṇe.",
  "anya-e0606n:247": "50.",
  "anya-e0606n:248": " Abhicāraka vidhikopita nisicāraka gahito,",
  "anya-e0606n:249": "Savisodana sahasādana pabhutīhi ca khalito;",
  "anya-e0606n:250": "Amariṁ bhavagahaṇe caraṁ ahaṁ appaṭisaraṇe,",
  "anya-e0606n:251": "Saraṇaṁ bhava bhagavaṁ mama bhavanīradhi taraṇe.",
  "anya-e0606n:252": "51.",
  "anya-e0606n:253": " Sayamevaca sajanopari kupito mativiyuto,",
  "anya-e0606n:254": "Savisādana galakantana pabhutāmita khalito;",
  "anya-e0606n:255": "Amariṁ bhavagahaṇe caraṁ ahaṁ appaṭisaraṇe,",
  "anya-e0606n:256": "Saraṇaṁ bhava bhagavaṁ mama bhavanīradhi taraṇe.",
  "anya-e0606n:257": "52.",
  "anya-e0606n:258": " Pitupūjana niratāsaya sukhito pitudayito,",
  "anya-e0606n:259": "Maraṇenaca pituno bhusaṁ anusocana nirato;",
  "anya-e0606n:260": "Parudiṁ ciraṁ atidussaha kasire bhavagahaṇe,",
  "anya-e0606n:261": "Saraṇaṁ bhava bhagavaṁ mama bhavanīradhi taraṇe.",
  "anya-e0606n:262": "53.",
  "anya-e0606n:263": " Samupaṭṭhiya janikaṁ nijaṁ abhivādana paramo,",
  "anya-e0606n:264": "Maraṇenaca janikāyanusaritā guṇamahimaṁ;",
  "anya-e0606n:265": "Parudiṁ ciraṁ atidussaha kasire bhavagahaṇe,",
  "anya-e0606n:266": "Saraṇaṁ bhava bhagavaṁ mama bhavanīradhi taraṇe.",
  "anya-e0606n:267": "54.",
  "anya-e0606n:268": " Gurudevata patimānana paramo piyasuvaco,",
  "anya-e0606n:269": "Samupāsitacaraṇo gurumaraṇenatidukhito;",
  "anya-e0606n:270": "Parudiṁ ciraṁ atidussaha kasire bhavagahaṇe,",
  "anya-e0606n:271": "Saraṇaṁ bhava bhagavaṁ mama bhavanīradhi taraṇe.",
  "anya-e0606n:272": "55.",
  "anya-e0606n:273": " Gharamedhitaṁ upagomitavibhavo ratibahulo,",
  "anya-e0606n:274": "Maraṇe piyabhariyāsutaduhitū’natikaruṇaṁ;",
  "anya-e0606n:275": "Parudiṁ ciraṁ atidussaha kasire bhavagahaṇe,",
  "anya-e0606n:276": "Saraṇaṁ bhava bhagavaṁ mama bhavanīradhi taraṇe.",
  "anya-e0606n:277": "56.",
  "anya-e0606n:278": " Piyapubbaja sahajānuja bhagiṇidvaya maraṇe,",
  "anya-e0606n:279": "Nija bandhava-sakha-sissaka maraṇe pyatikaruṇaṁ;",
  "anya-e0606n:280": "Parudiṁ ciraṁ atidussaha kasire bhavagahaṇe,",
  "anya-e0606n:281": "Saraṇaṁ bhava bhagavaṁ mama bhavanīradhi taraṇe.",
  "anya-e0606n:282": "57.",
  "anya-e0606n:283": " Jagatīpati gahaṇā ripujanatakkara haraṇā,",
  "anya-e0606n:284": "Saritodaka vahaṇā puthujalitānala dahaṇā;",
  "anya-e0606n:285": "Parudiṁ hatavibhavo ciramiha duggatigahaṇe,",
  "anya-e0606n:286": "Saraṇaṁ bhava bhagavaṁ mama bhavanīradhi taraṇe.",
  "anya-e0606n:287": "58.",
  "anya-e0606n:288": " Atiduggama visamākula bhavasaṅkaṭa patite,",
  "anya-e0606n:289": "Byasanaṁ ciramiti dussahaṁ anubhūyapi vimitaṁ;",
  "anya-e0606n:290": "Na jahe sukhalavavañcitahadayo bhavatasinaṁ,",
  "anya-e0606n:291": "Tamapākuru karuṇānidhi tasinaṁ mama kasiṇaṁ.",
  "anya-e0606n:292": "59.",
  "anya-e0606n:293": " Jananāvadhi maraṇaṁ viya maraṇāvadhi jananaṁ,",
  "anya-e0606n:294": "Ubhayenapi bhayamevahi bhavato mama niyataṁ;",
  "anya-e0606n:295": "Sivamevaca jananāvadhi maraṇāvadhi rahitaṁ,",
  "anya-e0606n:296": "Dada me sivaṁ amatandada taṁ anāsava bhagavaṁ.",
  "anya-e0606n:297": "60.",
  "anya-e0606n:298": " Ciradikkhitamapi me manaṁ anivārita tasinaṁ,",
  "anya-e0606n:299": "Bhavato bhava ratipīlitaṁ ahaho kalighaṭitaṁ!",
  "anya-e0606n:300": "Tamato pita bhayato mamamava me bhava saraṇaṁ,",
  "anya-e0606n:301": "Bhagavaṁ paṭisaraṇaṁ mama bhavanīradhi taraṇaṁ."
}