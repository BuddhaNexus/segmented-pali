{
  "snp1.2:0.1": "Sutta Nipāta 1.2 ",
  "snp1.2:0.2": "Dhaniyasutta ",
  "snp1.2:1.1": "“Pakkodano duddhakhīrohamasmi, ",
  "snp1.2:1.2": "(iti dhaniyo gopo) ",
  "snp1.2:1.3": "Anutīre mahiyā samānavāso; ",
  "snp1.2:1.4": "Channā kuṭi āhito gini, ",
  "snp1.2:1.5": "Atha ce patthayasī pavassa deva”. ",
  "snp1.2:2.1": "“Akkodhano vigatakhilohamasmi, ",
  "snp1.2:2.2": "(iti bhagavā) ",
  "snp1.2:2.3": "Anutīre mahiyekarattivāso; ",
  "snp1.2:2.4": "Vivaṭā kuṭi nibbuto gini, ",
  "snp1.2:2.5": "Atha ce patthayasī pavassa deva”. ",
  "snp1.2:3.1": "“Andhakamakasā na vijjare, ",
  "snp1.2:3.2": "(iti dhaniyo gopo) ",
  "snp1.2:3.3": "Kacche rūḷhatiṇe caranti gāvo; ",
  "snp1.2:3.4": "Vuṭṭhimpi saheyyumāgataṁ, ",
  "snp1.2:3.5": "Atha ce patthayasī pavassa deva”. ",
  "snp1.2:4.1": "“Baddhāsi bhisī susaṅkhatā, ",
  "snp1.2:4.2": "(iti bhagavā) ",
  "snp1.2:4.3": "Tiṇṇo pāragato vineyya oghaṁ; ",
  "snp1.2:4.4": "Attho bhisiyā na vijjati, ",
  "snp1.2:4.5": "Atha ce patthayasī pavassa deva”. ",
  "snp1.2:5.1": "“Gopī mama assavā alolā, ",
  "snp1.2:5.2": "(iti dhaniyo gopo) ",
  "snp1.2:5.3": "Dīgharattaṁ saṁvāsiyā manāpā; ",
  "snp1.2:5.4": "Tassā na suṇāmi kiñci pāpaṁ, ",
  "snp1.2:5.5": "Atha ce patthayasī pavassa deva”. ",
  "snp1.2:6.1": "“Cittaṁ mama assavaṁ vimuttaṁ, ",
  "snp1.2:6.2": "(iti bhagavā) ",
  "snp1.2:6.3": "Dīgharattaṁ paribhāvitaṁ sudantaṁ; ",
  "snp1.2:6.4": "Pāpaṁ pana me na vijjati, ",
  "snp1.2:6.5": "Atha ce patthayasī pavassa deva”. ",
  "snp1.2:7.1": "“Attavetanabhatohamasmi, ",
  "snp1.2:7.2": "(iti dhaniyo gopo) ",
  "snp1.2:7.3": "Puttā ca me samāniyā arogā; ",
  "snp1.2:7.4": "Tesaṁ na suṇāmi kiñci pāpaṁ, ",
  "snp1.2:7.5": "Atha ce patthayasī pavassa deva”. ",
  "snp1.2:8.1": "“Nāhaṁ bhatakosmi kassaci, ",
  "snp1.2:8.2": "(iti bhagavā) ",
  "snp1.2:8.3": "Nibbiṭṭhena carāmi sabbaloke; ",
  "snp1.2:8.4": "Attho bhatiyā na vijjati, ",
  "snp1.2:8.5": "Atha ce patthayasī pavassa deva”. ",
  "snp1.2:9.1": "“Atthi vasā atthi dhenupā, ",
  "snp1.2:9.2": "(iti dhaniyo gopo) ",
  "snp1.2:9.3": "Godharaṇiyo paveṇiyopi atthi; ",
  "snp1.2:9.4": "Usabhopi gavampatīdha atthi, ",
  "snp1.2:9.5": "Atha ce patthayasī pavassa deva”. ",
  "snp1.2:10.1": "“Natthi vasā natthi dhenupā, ",
  "snp1.2:10.2": "(iti bhagavā) ",
  "snp1.2:10.3": "Godharaṇiyo paveṇiyopi natthi; ",
  "snp1.2:10.4": "Usabhopi gavampatīdha natthi, ",
  "snp1.2:10.5": "Atha ce patthayasī pavassa deva”. ",
  "snp1.2:11.1": "“Khilā nikhātā asampavedhī, ",
  "snp1.2:11.2": "(iti dhaniyo gopo) ",
  "snp1.2:11.3": "Dāmā muñjamayā navā susaṇṭhānā; ",
  "snp1.2:11.4": "Na hi sakkhinti dhenupāpi chettuṁ, ",
  "snp1.2:11.5": "Atha ce patthayasī pavassa deva”. ",
  "snp1.2:12.1": "“Usabhoriva chetva bandhanāni, ",
  "snp1.2:12.2": "(iti bhagavā) ",
  "snp1.2:12.3": "Nāgo pūtilataṁva dālayitvā; ",
  "snp1.2:12.4": "Nāhaṁ punupessaṁ gabbhaseyyaṁ, ",
  "snp1.2:12.5": "Atha ce patthayasī pavassa deva”. ",
  "snp1.2:13.1": "Ninnañca thalañca pūrayanto, ",
  "snp1.2:13.2": "Mahāmegho pavassi tāvadeva; ",
  "snp1.2:13.3": "Sutvā devassa vassato, ",
  "snp1.2:13.4": "Imamatthaṁ dhaniyo abhāsatha. ",
  "snp1.2:14.1": "“Lābhā vata no anappakā, ",
  "snp1.2:14.2": "Ye mayaṁ bhagavantaṁ addasāma; ",
  "snp1.2:14.3": "Saraṇaṁ taṁ upema cakkhuma, ",
  "snp1.2:14.4": "Satthā no hohi tuvaṁ mahāmuni. ",
  "snp1.2:15.1": "Gopī ca ahañca assavā, ",
  "snp1.2:15.2": "Brahmacariyaṁ sugate carāmase; ",
  "snp1.2:15.3": "Jātimaraṇassa pāragū, ",
  "snp1.2:15.4": "Dukkhassantakarā bhavāmase”. ",
  "snp1.2:16.1": "“Nandati puttehi puttimā, ",
  "snp1.2:16.2": "(iti māro pāpimā) ",
  "snp1.2:16.3": "Gomā gohi tatheva nandati; ",
  "snp1.2:16.4": "Upadhī hi narassa nandanā, ",
  "snp1.2:16.5": "Na hi so nandati yo nirūpadhi”. ",
  "snp1.2:17.1": "“Socati puttehi puttimā, ",
  "snp1.2:17.2": "(iti bhagavā) ",
  "snp1.2:17.3": "Gomā gohi tatheva socati; ",
  "snp1.2:17.4": "Upadhī hi narassa socanā, ",
  "snp1.2:17.5": "Na hi so socati yo nirūpadhī”ti. ",
  "snp1.2:17.6": "Dhaniyasuttaṁ dutiyaṁ. "
}