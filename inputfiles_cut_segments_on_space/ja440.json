{
  "ja440:0.1": "Jātaka ",
  "ja440:0.2": "Dasakanipāta ",
  "ja440:0.3": "Catudvāravagga ",
  "ja440:0.4": "2. Kaṇhajātaka ",
  "ja440:1.1": "“Kaṇho vatāyaṁ puriso, ",
  "ja440:1.2": "kaṇhaṁ bhuñjati bhojanaṁ; ",
  "ja440:1.3": "Kaṇhe bhūmipadesasmiṁ, ",
  "ja440:1.4": "na mayhaṁ manaso piyo”. ",
  "ja440:2.1": "“Na kaṇho tacasā hoti, ",
  "ja440:2.2": "antosāro hi brāhmaṇo; ",
  "ja440:2.3": "Yasmiṁ pāpāni kammāni, ",
  "ja440:2.4": "sa ve kaṇho sujampati”. ",
  "ja440:3.1": "“Etasmiṁ te sulapite, ",
  "ja440:3.2": "patirūpe subhāsite; ",
  "ja440:3.3": "Varaṁ brāhmaṇa te dammi, ",
  "ja440:3.4": "yaṁ kiñci manasicchasi”. ",
  "ja440:4.1": "“Varañce me ado sakka, ",
  "ja440:4.2": "sabbabhūtānamissara; ",
  "ja440:4.3": "Sunikkodhaṁ suniddosaṁ, ",
  "ja440:4.4": "nillobhaṁ vuttimattano; ",
  "ja440:4.5": "Nisnehamabhikaṅkhāmi, ",
  "ja440:4.6": "ete me caturo vare”. ",
  "ja440:5.1": "“Kiṁ nu kodhe vā dose vā, ",
  "ja440:5.2": "lobhe snehe ca brāhmaṇa; ",
  "ja440:5.3": "Ādīnavaṁ tvaṁ passasi, ",
  "ja440:5.4": "taṁ me akkhāhi pucchito”. ",
  "ja440:6.1": "“Appo hutvā bahu hoti, ",
  "ja440:6.2": "vaḍḍhate so akhantijo; ",
  "ja440:6.3": "Āsaṅgī bahupāyāso, ",
  "ja440:6.4": "tasmā kodhaṁ na rocaye. ",
  "ja440:7.1": "Duṭṭhassa pharusā vācā, ",
  "ja440:7.2": "parāmāso anantarā; ",
  "ja440:7.3": "Tato pāṇi tato daṇḍo, ",
  "ja440:7.4": "satthassa paramā gati; ",
  "ja440:7.5": "Doso kodhasamuṭṭhāno, ",
  "ja440:7.6": "tasmā dosaṁ na rocaye. ",
  "ja440:8.1": "Ālopa sāhasākārā, ",
  "ja440:8.2": "nikatī vañcanāni ca; ",
  "ja440:8.3": "Dissanti lobhadhammesu, ",
  "ja440:8.4": "tasmā lobhaṁ na rocaye. ",
  "ja440:9.1": "Snehasaṅgathitā ganthā, ",
  "ja440:9.2": "senti manomayā puthū; ",
  "ja440:9.3": "Te bhusaṁ upatāpenti, ",
  "ja440:9.4": "tasmā snehaṁ na rocaye”. ",
  "ja440:10.1": "“Etasmiṁ te sulapite, ",
  "ja440:10.2": "patirūpe subhāsite; ",
  "ja440:10.3": "Varaṁ brāhmaṇa te dammi, ",
  "ja440:10.4": "yaṁ kiñci manasicchasi”. ",
  "ja440:11.1": "“Varañce me ado sakka, ",
  "ja440:11.2": "sabbabhūtānamissara; ",
  "ja440:11.3": "Araññe me viharato, ",
  "ja440:11.4": "niccaṁ ekavihārino; ",
  "ja440:11.5": "Ābādhā mā uppajjeyyuṁ, ",
  "ja440:11.6": "antarāyakarā bhusā”. ",
  "ja440:12.1": "“Etasmiṁ te sulapite, ",
  "ja440:12.2": "patirūpe subhāsite; ",
  "ja440:12.3": "Varaṁ brāhmaṇa te dammi, ",
  "ja440:12.4": "yaṁ kiñci manasicchasi”. ",
  "ja440:13.1": "“Varañce me ado sakka, ",
  "ja440:13.2": "sabbabhūtānamissara; ",
  "ja440:13.3": "Na mano vā sarīraṁ vā, ",
  "ja440:13.4": "maṁ-kate sakka kassaci; ",
  "ja440:13.5": "Kadāci upahaññetha, ",
  "ja440:13.6": "etaṁ sakka varaṁ vare”ti. ",
  "ja440:14.1": "Kaṇhajātakaṁ dutiyaṁ. "
}