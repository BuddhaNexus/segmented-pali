{
  "anya-e1007n2:0": "Cāṇakyanītilā gāthā",
  "anya-e1007n2:1": "Lālane bahavo dosā,",
  "anya-e1007n2:2": "Tāḷane bahavo guṇā;",
  "anya-e1007n2:3": "Tasmā puttañca sissañca,",
  "anya-e1007n2:4": "Tāḷaye na ca lālaye.",
  "anya-e1007n2:5": "28.",
  "anya-e1007n2:6": "Atītassa hi mittassa,",
  "anya-e1007n2:7": "Yo ce dosaṁ pakāsaye;",
  "anya-e1007n2:8": "So have paccuppannassa,",
  "anya-e1007n2:9": "Dosaṁ bhāseti ñāyati.",
  "anya-e1007n2:10": "29.",
  "anya-e1007n2:11": "Latāviya sevakā te,",
  "anya-e1007n2:12": "Ye nissayaṁ palambare;",
  "anya-e1007n2:13": "Nissayassa vināsena,",
  "anya-e1007n2:14": "Bhūmyaṁ senti anāthakā.",
  "anya-e1007n2:15": "30.",
  "anya-e1007n2:16": "Dosasiṅgehi vijjhanto,",
  "anya-e1007n2:17": "Mānakhūrehi akkamaṁ;",
  "anya-e1007n2:18": "Bhayaṁ karoti lokamhi,",
  "anya-e1007n2:19": "Gova bālo vihiṁsako.",
  "anya-e1007n2:20": "31.",
  "anya-e1007n2:21": "Ādo upari lokoyaṁ,",
  "anya-e1007n2:22": "Ujulekhāya tiṭṭhati;",
  "anya-e1007n2:23": "Musāvātehi taṁlokaṁ,",
  "anya-e1007n2:24": "Nipātesi anajjavaṁ.",
  "anya-e1007n2:25": "32.",
  "anya-e1007n2:26": "Sughaṭaṁ kumbhakārena,",
  "anya-e1007n2:27": "Nāraho paribhuñjituṁ;",
  "anya-e1007n2:28": "Tathūpamāya vekkheyya,",
  "anya-e1007n2:29": "Sakammaparakammani.",
  "anya-e1007n2:30": "33.",
  "anya-e1007n2:31": "Anantaraṁsī sūropi,",
  "anya-e1007n2:32": "Nasakkoti ghanaṁ tamaṁ;",
  "anya-e1007n2:33": "Vijjhituṁ raṁsiyā loke,",
  "anya-e1007n2:34": "Tathā madanamohitā;",
  "anya-e1007n2:35": "Nasakkonti madaṁ bhetvā,",
  "anya-e1007n2:36": "Paññābhāya pabhāsituṁ.",
  "anya-e1007n2:37": "34.",
  "anya-e1007n2:38": "Khedaveraṁ daliddamhi,",
  "anya-e1007n2:39": "Bhogimhi rogupaddavaṁ;",
  "anya-e1007n2:40": "Dessaverañca āṇimhi,",
  "anya-e1007n2:41": "Passe lokassa veritaṁ.",
  "anya-e1007n2:42": "35.",
  "anya-e1007n2:43": "Saṁladdhena subhogena,",
  "anya-e1007n2:44": "Jīvaṁ suddhaṁ kare nijaṁ;",
  "anya-e1007n2:45": "Seṭṭho so tena jīvena,",
  "anya-e1007n2:46": "Jeguccho malajīviko.",
  "anya-e1007n2:47": "36.",
  "anya-e1007n2:48": "Vajira puppharāgānaṁ,",
  "anya-e1007n2:49": "Visesaṁ yo nabujjhati;",
  "anya-e1007n2:50": "Kathañhi so vikkīṇeyya,",
  "anya-e1007n2:51": "Kīṇeyya vā yathātathaṁ.",
  "anya-e1007n2:52": "37.",
  "anya-e1007n2:53": "Kippīli kopi cintetvā,",
  "anya-e1007n2:54": "Pabbataṁ bhettu mussahaṁ;",
  "anya-e1007n2:55": "Abalā tanumajjhattā,",
  "anya-e1007n2:56": "Cintā hasyāva sā mudhā.",
  "anya-e1007n2:57": "38.",
  "anya-e1007n2:58": "Jātamattaṁ na yo sattuṁ,",
  "anya-e1007n2:59": "Rogañcūpasamaṁ naye;",
  "anya-e1007n2:60": "Mahābalopi teneva,",
  "anya-e1007n2:61": "Vuddhiṁpatvā sa haññate.",
  "anya-e1007n2:62": "39.",
  "anya-e1007n2:63": "Sajīvamaṁsabhakkhehi,",
  "anya-e1007n2:64": "Sadāṭhīhi mukhehi bho;",
  "anya-e1007n2:65": "Biḷārabyagghasīhānaṁ,",
  "anya-e1007n2:66": "Nihīnāni anekadhā;",
  "anya-e1007n2:67": "Tikkhāni kharavādāni,",
  "anya-e1007n2:68": "Manussānaṁ mukhāni ve.",
  "anya-e1007n2:69": "40.",
  "anya-e1007n2:70": "Viluppantā vidhāvanti,",
  "anya-e1007n2:71": "Sajīvavuttikammunā;",
  "anya-e1007n2:72": "Janā tena vihaññanti,",
  "anya-e1007n2:73": "Caranti dhammavemukhā.",
  "anya-e1007n2:74": "41.",
  "anya-e1007n2:75": "Sulabhaṁ lokiyaṁ loke,",
  "anya-e1007n2:76": "Sāsanīyaṁva dullabhaṁ;",
  "anya-e1007n2:77": "Dullabhaṁ taṁ vamaññanto,",
  "anya-e1007n2:78": "Eso bālatamo bhave.",
  "anya-e1007n2:79": "42.",
  "anya-e1007n2:80": "Yo patittha agyāvāṭaṁ,",
  "anya-e1007n2:81": "Mohā taṁ upakārituṁ;",
  "anya-e1007n2:82": "Aññorohi tadā vāṭaṁ,",
  "anya-e1007n2:83": "Dutīyo muḷhamuḷhako.",
  "anya-e1007n2:84": "43.",
  "anya-e1007n2:85": "Byaggho āvudhaviddho hi,",
  "anya-e1007n2:86": "Akā duṭṭhāni ninnadaṁ;",
  "anya-e1007n2:87": "Tatheva sādhusatthena,",
  "anya-e1007n2:88": "Viddho bālo pakuppito.",
  "anya-e1007n2:89": "44.",
  "anya-e1007n2:90": "Pivanti lohitaṁ ḍaṁsā,",
  "anya-e1007n2:91": "Anto tuṇḍena makkhikā;",
  "anya-e1007n2:92": "Bahiddhā parivārenti,",
  "anya-e1007n2:93": "Jano tena ḍaṁsāyaye.",
  "anya-e1007n2:94": "45.",
  "anya-e1007n2:95": "Adhanassa khaṇo appo,",
  "anya-e1007n2:96": "Saddhammo appakālino;",
  "anya-e1007n2:97": "Appako tena yuñjeyyuṁ,",
  "anya-e1007n2:98": "Khaṇaṁ bahuṁ labhetave."
}