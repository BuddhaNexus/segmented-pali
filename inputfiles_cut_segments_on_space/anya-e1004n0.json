{
  "anya-e1004n0:0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1004n0:1": "Mahārahanīti",
  "anya-e1004n0:2": "Paṇḍitakathā",
  "anya-e1004n0:3": "1.",
  "anya-e1004n0:4": "Mahāraharahaṁ sakya,",
  "anya-e1004n0:5": "Muniṁ nīvaraṇā taṇhā;",
  "anya-e1004n0:6": "Muttaṁ muttaṁ sudassanaṁ,",
  "anya-e1004n0:7": "Vande bodhivaraṁ varaṁ.",
  "anya-e1004n0:8": "2.",
  "anya-e1004n0:9": "Muninā munināgena,",
  "anya-e1004n0:10": "Duṭṭhapabbajjitā jitā;",
  "anya-e1004n0:11": "Yathā yathāasaddhamma-",
  "anya-e1004n0:12": "Puraṇā puraṇādayo.",
  "anya-e1004n0:13": "3.",
  "anya-e1004n0:14": "Tathā tathāgatovādā,",
  "anya-e1004n0:15": "Nugate lokanītiyaṁ;",
  "anya-e1004n0:16": "Katayogena vidūnā,",
  "anya-e1004n0:17": "Jeyyovānattha kārino.",
  "anya-e1004n0:18": "4.",
  "anya-e1004n0:19": "Nitīdha jantunaṁ sāro,",
  "anya-e1004n0:20": "Mittācariyāca pītarā;",
  "anya-e1004n0:21": "Nītimā subuddhi byatto,",
  "anya-e1004n0:22": "Sutavā atthadassimā.",
  "anya-e1004n0:23": "5.",
  "anya-e1004n0:24": "Kaviheraññakā katvā,",
  "anya-e1004n0:25": "Suttattaṁ satthakañcanaṁ;",
  "anya-e1004n0:26": "Gajjapajjādyālaṅkāraṁ,",
  "anya-e1004n0:27": "Karontīdha manorammaṁ.",
  "anya-e1004n0:28": "6.",
  "anya-e1004n0:29": "Vutyaṁ visadañāṇassa,",
  "anya-e1004n0:30": "Ñāto atthotarassa na;",
  "anya-e1004n0:31": "Surappabhāya ādāso,",
  "anya-e1004n0:32": "Chāyaṁ disse na mākare.",
  "anya-e1004n0:33": "7.",
  "anya-e1004n0:34": "Mahātejopi tejoyaṁ,",
  "anya-e1004n0:35": "Mattikaṁ namudukaṁkaro;",
  "anya-e1004n0:36": "Āpo āpeti mudukaṁ,",
  "anya-e1004n0:37": "Sādhuvācāca kakkhaḷaṁ.",
  "anya-e1004n0:38": "8.",
  "anya-e1004n0:39": "Mudunāva ripuṁ jeti,",
  "anya-e1004n0:40": "Mudunā jeti dāruṇaṁ;",
  "anya-e1004n0:41": "Nosiddhaṁ mudunā kiñci,",
  "anya-e1004n0:42": "Yatvato mudunā jaye.",
  "anya-e1004n0:43": "9.",
  "anya-e1004n0:44": "Candanaṁ sītalaṁ loke,",
  "anya-e1004n0:45": "Tato candaṁva sītalaṁ;",
  "anya-e1004n0:46": "Candanacandasītamhā,",
  "anya-e1004n0:47": "Vākyaṁ sādhusubhāsitaṁ.",
  "anya-e1004n0:48": "10.",
  "anya-e1004n0:49": "Pattakalloditaṁ appaṁ,",
  "anya-e1004n0:50": "Vākyaṁ subhāsitaṁ bhave;",
  "anya-e1004n0:51": "Khudditassa kadannaṁpi,",
  "anya-e1004n0:52": "Suttaṁ sāduraso siyā.",
  "anya-e1004n0:53": "11.",
  "anya-e1004n0:54": "Satthakāpi bahūvācā,",
  "anya-e1004n0:55": "Nādarā bahubhāṇino;",
  "anya-e1004n0:56": "Sopakāramudāsīnaṁ,",
  "anya-e1004n0:57": "Nanudiṭṭhaṁ nadījalaṁ.",
  "anya-e1004n0:58": "12.",
  "anya-e1004n0:59": "Saṁsāravisarukkhassa,",
  "anya-e1004n0:60": "Dvayamevāmataṁphalaṁ;",
  "anya-e1004n0:61": "Subhāsitassasārādo,",
  "anya-e1004n0:62": "Sādhūhirasamāgamo.",
  "anya-e1004n0:63": "13.",
  "anya-e1004n0:64": "Pāsāṇachattaṁ garukaṁ,",
  "anya-e1004n0:65": "Tato devānacikkhanā;",
  "anya-e1004n0:66": "Tato vuḍḍhānamovādo,",
  "anya-e1004n0:67": "Tato buddhassa sāsanaṁ;",
  "anya-e1004n0:68": "14.",
  "anya-e1004n0:69": "Tulaṁ sallahukaṁ loke,",
  "anya-e1004n0:70": "Tato capalajātiko;",
  "anya-e1004n0:71": "Tatonovādiko tato,",
  "anya-e1004n0:72": "Yati dhammepamādako.",
  "anya-e1004n0:73": "15.",
  "anya-e1004n0:74": "Aha gacchati hāyantī,",
  "anya-e1004n0:75": "Sattānamidha jīvitaṁ;",
  "anya-e1004n0:76": "Tasmāhi māpamattatvaṁ,",
  "anya-e1004n0:77": "Gacchantu jinasāsane.",
  "anya-e1004n0:78": "16.",
  "anya-e1004n0:79": "Paṇḍitassa pasaṁsāya,",
  "anya-e1004n0:80": "Daṇḍobālena dīyate;",
  "anya-e1004n0:81": "Paṇḍito paṇḍiteneva,",
  "anya-e1004n0:82": "Vaṇṇitova suvaṇṇito.",
  "anya-e1004n0:83": "17.",
  "anya-e1004n0:84": "Satesu jāyate sūro,",
  "anya-e1004n0:85": "Sahassesuca paṇḍito;",
  "anya-e1004n0:86": "Vuttā satasahassesu,",
  "anya-e1004n0:87": "Dātā bhavativā navā.",
  "anya-e1004n0:88": "18.",
  "anya-e1004n0:89": "Bhūpattaṁca paṇḍitattañca,",
  "anya-e1004n0:90": "Nevatulyaṁ kadācipi;",
  "anya-e1004n0:91": "Sadesepūjito rājā,",
  "anya-e1004n0:92": "Buddho sabbatthapūjito.",
  "anya-e1004n0:93": "19.",
  "anya-e1004n0:94": "Kvātibhāro samatthānaṁ,",
  "anya-e1004n0:95": "Kiṁdūro byavahārinaṁ;",
  "anya-e1004n0:96": "Kovideso savijjānaṁ,",
  "anya-e1004n0:97": "Koparo piyavādinaṁ.",
  "anya-e1004n0:98": "20.",
  "anya-e1004n0:99": "Nanu te yevasantā no,",
  "anya-e1004n0:100": "Sāgarā nakulācalā;",
  "anya-e1004n0:101": "Appampi mariyādaṁ ye,",
  "anya-e1004n0:102": "Saṁvaṭṭevi jahantino.",
  "anya-e1004n0:103": "21.",
  "anya-e1004n0:104": "Palayetītamariyādā,",
  "anya-e1004n0:105": "Bhavantikira sāgarā;",
  "anya-e1004n0:106": "Mariyādātikaṁ sādhu,",
  "anya-e1004n0:107": "Naicchanti palayepi.",
  "anya-e1004n0:108": "22.",
  "anya-e1004n0:109": "Sataṁ dīghāyukaṁ sabba,",
  "anya-e1004n0:110": "Sattānaṁ sukhakāraṇaṁ;",
  "anya-e1004n0:111": "Asataṁpana sabbesaṁ,",
  "anya-e1004n0:112": "Dukkhahetu nasaṁsayo.",
  "anya-e1004n0:113": "23.",
  "anya-e1004n0:114": "Pāmokkhe sujane sante,",
  "anya-e1004n0:115": "Sabbepi sujanā janā;",
  "anya-e1004n0:116": "Jātekasmiṁsāragandhe,",
  "anya-e1004n0:117": "Sabbe gandhamayā dumā.",
  "anya-e1004n0:118": "24.",
  "anya-e1004n0:119": "Attanā yadi ekena,",
  "anya-e1004n0:120": "Vinatena mahājanā;",
  "anya-e1004n0:121": "Vinayaṁ yanti sabbepi,",
  "anya-e1004n0:122": "Ko taṁ nāseyya paṇḍito.",
  "anya-e1004n0:123": "25.",
  "anya-e1004n0:124": "Sarīrassa guṇānañca,",
  "anya-e1004n0:125": "Dūramaccanta mantaraṁ;",
  "anya-e1004n0:126": "Sarīraṁ khaṇaviddhaṁsi,",
  "anya-e1004n0:127": "Kappantiṭṭhāyino guṇā.",
  "anya-e1004n0:128": "26.",
  "anya-e1004n0:129": "Yadi niccamaniccena,",
  "anya-e1004n0:130": "Nimalaṁ malavāhinā;",
  "anya-e1004n0:131": "Yo so kāyena labhetha,",
  "anya-e1004n0:132": "Kiṁnaladdhaṁ bhave nukiṁ.",
  "anya-e1004n0:133": "27.",
  "anya-e1004n0:134": "Dhammatthakāmamokkhānaṁ,",
  "anya-e1004n0:135": "Pāṇaṁ saṁsiddhikāraṇaṁ;",
  "anya-e1004n0:136": "Tannighātā kinnahataṁ,",
  "anya-e1004n0:137": "Rakkhanā kinnarakkhitaṁ.",
  "anya-e1004n0:138": "28.",
  "anya-e1004n0:139": "Attā bandhu manussassa,",
  "anya-e1004n0:140": "Ripu attāva jantunaṁ;",
  "anya-e1004n0:141": "Attāva niyato ñāti,",
  "anya-e1004n0:142": "Attāva niyato ripu.",
  "anya-e1004n0:143": "29.",
  "anya-e1004n0:144": "Attanoparicāgena,",
  "anya-e1004n0:145": "Yaṁsitamanurakkhanaṁ;",
  "anya-e1004n0:146": "Karonti sajjanāyeva,",
  "anya-e1004n0:147": "Na taṁ nītimatā mataṁ.",
  "anya-e1004n0:148": "30.",
  "anya-e1004n0:149": "Tiṇāni bhūmirudakaṁ,",
  "anya-e1004n0:150": "Catutthī vākyasuṭṭhutā;",
  "anya-e1004n0:151": "Etāni hi sataṁgehe,",
  "anya-e1004n0:152": "Nochindante kadācipi.",
  "anya-e1004n0:153": "31.",
  "anya-e1004n0:154": "Ambuṁ pivantino najjo,",
  "anya-e1004n0:155": "Rukkho khādatino phalaṁ;",
  "anya-e1004n0:156": "Megho kvacipi no sassaṁ,",
  "anya-e1004n0:157": "Paratthāya sataṁdhanaṁ.",
  "anya-e1004n0:158": "32.",
  "anya-e1004n0:159": "Sataṁ pharusavācāhi,",
  "anya-e1004n0:160": "Nayāti vikatiṁ mano;",
  "anya-e1004n0:161": "Tiṇukkāhi nasakkāva,",
  "anya-e1004n0:162": "Tāpetuṁ sāgare jalaṁ.",
  "anya-e1004n0:163": "33.",
  "anya-e1004n0:164": "Sampatyaṁ mahattaṁceto,",
  "anya-e1004n0:165": "Bhavatyuppalakomalaṁ;",
  "anya-e1004n0:166": "Vippatyañca mahāsele,",
  "anya-e1004n0:167": "Silāsaṅghādakakkaso.",
  "anya-e1004n0:168": "34.",
  "anya-e1004n0:169": "Atthaṁ mahantamāpajja,",
  "anya-e1004n0:170": "Vijjaṁ sampattimevaca;",
  "anya-e1004n0:171": "Careyyāmānathaddhoyo,",
  "anya-e1004n0:172": "Paṇḍito so pavuccati.",
  "anya-e1004n0:173": "35.",
  "anya-e1004n0:174": "Nālabbhamabhipatthenti,",
  "anya-e1004n0:175": "Naṭṭhampi nacasocare;",
  "anya-e1004n0:176": "Vippatyañca namuyhanti,",
  "anya-e1004n0:177": "Yenarā teva paṇḍito.",
  "anya-e1004n0:178": "36.",
  "anya-e1004n0:179": "Gaṇṭhiṭṭhāne ekapade,",
  "anya-e1004n0:180": "Nātimaññeyya paṇḍito;",
  "anya-e1004n0:181": "Kiṁ akko veḷupabbhāro,",
  "anya-e1004n0:182": "Timahādīpabhānudo.",
  "anya-e1004n0:183": "37.",
  "anya-e1004n0:184": "Guṇadosesumekena,",
  "anya-e1004n0:185": "Natthi koci vivajjito;",
  "anya-e1004n0:186": "Sukhumālassa padumassa,",
  "anya-e1004n0:187": "Naḷā bhavati kakkasā.",
  "anya-e1004n0:188": "38.",
  "anya-e1004n0:189": "Sumahantāni satthāni,",
  "anya-e1004n0:190": "Dhārayanto bahussutā;",
  "anya-e1004n0:191": "Chettāro saṁsayānantu,",
  "anya-e1004n0:192": "Kilante lobhamohatā.",
  "anya-e1004n0:193": "39.",
  "anya-e1004n0:194": "Dosampi saguṇe disvā,",
  "anya-e1004n0:195": "Guṇavādī vadantino;",
  "anya-e1004n0:196": "Na loko vijjamānampi,",
  "anya-e1004n0:197": "Cande passati lañchanaṁ.",
  "anya-e1004n0:198": "40.",
  "anya-e1004n0:199": "Nipuṇe sutameseyya,",
  "anya-e1004n0:200": "Vicinitvā sutatthiko;",
  "anya-e1004n0:201": "Bhattaṁ hukkhaliyaṁ pakkaṁ,",
  "anya-e1004n0:202": "Bhājanaṁpi tathā bhave.",
  "anya-e1004n0:203": "41.",
  "anya-e1004n0:204": "Guṇā kubbanti dūtattaṁ,",
  "anya-e1004n0:205": "Dūrepi vasataṁ sataṁ;",
  "anya-e1004n0:206": "Ketake gandha ghāsāya,",
  "anya-e1004n0:207": "Gacchanti bhamarā sayaṁ.",
  "anya-e1004n0:208": "42.",
  "anya-e1004n0:209": "Paṇḍito sutasampanno,",
  "anya-e1004n0:210": "Yatthaatthīti cesuto;",
  "anya-e1004n0:211": "Mahussāhena taṁṭhānaṁ,",
  "anya-e1004n0:212": "Gantabbaṁ sutaresinā.",
  "anya-e1004n0:213": "43.",
  "anya-e1004n0:214": "Sabbasutaṁ madhīyetha,",
  "anya-e1004n0:215": "Hīnamukkaṭṭha majjhimaṁ;",
  "anya-e1004n0:216": "Sabbassaatthaṁ jāneyya,",
  "anya-e1004n0:217": "Naca sabbaṁ payojaye;",
  "anya-e1004n0:218": "Hoti tādisakokālo,",
  "anya-e1004n0:219": "Yattha atthāvahaṁ sutaṁ.",
  "anya-e1004n0:220": "44.",
  "anya-e1004n0:221": "Ṭhito cirattanaṁ nīre,",
  "anya-e1004n0:222": "Sakkharo nantaraṁsuto;",
  "anya-e1004n0:223": "Dabbho nīrantike jāto,",
  "anya-e1004n0:224": "Sabbakālantaraṁsuto.",
  "anya-e1004n0:225": "45.",
  "anya-e1004n0:226": "Vasuṁ gaṇhanti dūraṭṭhā,",
  "anya-e1004n0:227": "Pabbate ratanocite;",
  "anya-e1004n0:228": "Na milakkhā samīpaṭṭhā,",
  "anya-e1004n0:229": "Evaṁ bālā bahussute.",
  "anya-e1004n0:230": "46.",
  "anya-e1004n0:231": "Hiraññena migānaṁva,",
  "anya-e1004n0:232": "Susīlaṁpi asīlino;",
  "anya-e1004n0:233": "Adhammikassa dhammena,",
  "anya-e1004n0:234": "Bālānaṁpi sutena kiṁ.",
  "anya-e1004n0:235": "47.",
  "anya-e1004n0:236": "Sutena hadayaṭṭhena,",
  "anya-e1004n0:237": "Khalo nevasusīlavā;",
  "anya-e1004n0:238": "Madhunā koṭaraṭṭhena,",
  "anya-e1004n0:239": "Nimbo kiṁ madhurāyate.",
  "anya-e1004n0:240": "48.",
  "anya-e1004n0:241": "Sakiṁpi viññu dhīrena,",
  "anya-e1004n0:242": "Karoti sahasaṅgamaṁ;",
  "anya-e1004n0:243": "Attatthañca paratthañca,",
  "anya-e1004n0:244": "Nibbānantaṁ sukhaṁ labhe.",
  "anya-e1004n0:245": "49.",
  "anya-e1004n0:246": "Haṁso majjhe nakākānaṁ,",
  "anya-e1004n0:247": "Sīho gunnaṁ nasobhati;",
  "anya-e1004n0:248": "Gadrabhānaṁ turaṅgono,",
  "anya-e1004n0:249": "Bālānañca napaṇḍito.",
  "anya-e1004n0:250": "50.",
  "anya-e1004n0:251": "Ākiṇṇopi asabbhīva,",
  "anya-e1004n0:252": "Asaṁsaṭṭhova bhaddako;",
  "anya-e1004n0:253": "Bahunā sannajātena,",
  "anya-e1004n0:254": "Gacche nummattakenidha.",
  "anya-e1004n0:255": "51.",
  "anya-e1004n0:256": "Nadītire khatekūpe,",
  "anya-e1004n0:257": "Araṇītālavaṇṭake;",
  "anya-e1004n0:258": "Navade āpādi natthīti,",
  "anya-e1004n0:259": "Mukheca vacanaṁ tathā.",
  "anya-e1004n0:260": "52.",
  "anya-e1004n0:261": "Sutasanniccayā dhīrā,",
  "anya-e1004n0:262": "Tuṇhibhūtā apucchitā;",
  "anya-e1004n0:263": "Puṇṇā subhāsitenāpi,",
  "anya-e1004n0:264": "Ghaṇḍādyaghaṭitāyathā.",
  "anya-e1004n0:265": "53.",
  "anya-e1004n0:266": "Apuṭṭho paṇḍito bherī,",
  "anya-e1004n0:267": "Pajjunno hoti pucchito;",
  "anya-e1004n0:268": "Bālo puṭṭho apuṭṭhoca,",
  "anya-e1004n0:269": "Bahuṁ vikattate sadā.",
  "anya-e1004n0:270": "54.",
  "anya-e1004n0:271": "Sampannaguṇā laṅkāro,",
  "anya-e1004n0:272": "Sabbasatta hitāvaho;",
  "anya-e1004n0:273": "Nacareyya parattatthaṁ,",
  "anya-e1004n0:274": "Kuto so paṇḍito bhave.",
  "anya-e1004n0:275": "55.",
  "anya-e1004n0:276": "Saparatthaṁ care dhīro,",
  "anya-e1004n0:277": "Asakkonto sakaṁ care;",
  "anya-e1004n0:278": "Tathāpica asakkonto,",
  "anya-e1004n0:279": "Pāpāttānaṁ viyojaye.",
  "anya-e1004n0:280": "56.",
  "anya-e1004n0:281": "Bālecummattake bhūpe,",
  "anya-e1004n0:282": "Gurumātāpitūsvapi;",
  "anya-e1004n0:283": "Nadosaṁ kariyā pañño,",
  "anya-e1004n0:284": "Saṅgheca jeṭṭhabhātari.",
  "anya-e1004n0:285": "57.",
  "anya-e1004n0:286": "Atthanāsaṁ manotāpaṁ,",
  "anya-e1004n0:287": "Ghareduccaritānica;",
  "anya-e1004n0:288": "Vañcanaṁ avamānañca,",
  "anya-e1004n0:289": "Matimā napakāsaye.",
  "anya-e1004n0:290": "58.",
  "anya-e1004n0:291": "Paradānaṁ janettīva,",
  "anya-e1004n0:292": "Leṭṭuva paradabbakaṁ;",
  "anya-e1004n0:293": "Attāva sabbabhūtāni,",
  "anya-e1004n0:294": "Yo passati so passati.",
  "anya-e1004n0:295": "59.",
  "anya-e1004n0:296": "Parūpavāde padhiro,",
  "anya-e1004n0:297": "Paravajjesvalocano;",
  "anya-e1004n0:298": "Paṅgulo aññanārīsu,",
  "anya-e1004n0:299": "Dussakappesvacetano.",
  "anya-e1004n0:300": "60.",
  "anya-e1004n0:301": "Sabbaṁ suṇāti sotena,",
  "anya-e1004n0:302": "Sabbaṁ passati cakkhunā;",
  "anya-e1004n0:303": "Nacadiṭṭhaṁ sutaṁ dhīro,",
  "anya-e1004n0:304": "Sabbaṁ uccitumarahati.",
  "anya-e1004n0:305": "61.",
  "anya-e1004n0:306": "Cakkhumassa yathāandho,",
  "anya-e1004n0:307": "Sotavā padhiroyathā;",
  "anya-e1004n0:308": "Paññāvāssa yathāmūgo,",
  "anya-e1004n0:309": "Balavā dubbaloriva;",
  "anya-e1004n0:310": "Atha atthesamuppanne,",
  "anya-e1004n0:311": "Sayetha matasāyitaṁ.",
  "anya-e1004n0:312": "62.",
  "anya-e1004n0:313": "Pāpamitte vivajjetvā,",
  "anya-e1004n0:314": "Bhajeyyu ttamapuggalaṁ;",
  "anya-e1004n0:315": "Ovādecassa tiṭṭheyya,",
  "anya-e1004n0:316": "Patthento acalaṁsukhaṁ.",
  "anya-e1004n0:317": "63.",
  "anya-e1004n0:318": "Sususā sutavaḍḍhanaṁ,",
  "anya-e1004n0:319": "Sutaṁ paññāyavaḍḍhanaṁ;",
  "anya-e1004n0:320": "Paññāya atthaṁ jānāti,",
  "anya-e1004n0:321": "Ñāto attho sukhāvaho.",
  "anya-e1004n0:322": "64.",
  "anya-e1004n0:323": "Pāmojja karaṇaṁ ṭhānaṁ,",
  "anya-e1004n0:324": "Pasaṁsāvahanaṁsukhaṁ;",
  "anya-e1004n0:325": "Phalānisaṁso bhāveti,",
  "anya-e1004n0:326": "Vahanto porissaṁdhuraṁ.",
  "anya-e1004n0:327": "65.",
  "anya-e1004n0:328": "Atisītaṁ atiuṇhaṁ,",
  "anya-e1004n0:329": "Atisāya midaṁahu;",
  "anya-e1004n0:330": "Iti visaṭṭha kammante,",
  "anya-e1004n0:331": "Khaṇā accanti māṇave.",
  "anya-e1004n0:332": "66.",
  "anya-e1004n0:333": "Yoca sītañca uṇhañca,",
  "anya-e1004n0:334": "Tiṇātiyo namaññati;",
  "anya-e1004n0:335": "Karaṁ purisa kiccāni,",
  "anya-e1004n0:336": "So sukhā navihāyati.",
  "anya-e1004n0:337": "67.",
  "anya-e1004n0:338": "Yasalābhaṁ jigīsantaṁ,",
  "anya-e1004n0:339": "Naraṁ vajjenti dūrato;",
  "anya-e1004n0:340": "Apatthetvāna te tasmā,",
  "anya-e1004n0:341": "Taṁmaggaṁ maggaye budho.",
  "anya-e1004n0:342": "68.",
  "anya-e1004n0:343": "Desamosajja gacchanti,",
  "anya-e1004n0:344": "Sīhā sappurisā gajā;",
  "anya-e1004n0:345": "Tattheva nidhanaṁ yanti,",
  "anya-e1004n0:346": "Kākā kāpurisā migā.",
  "anya-e1004n0:347": "69.",
  "anya-e1004n0:348": "Yasmiṁdese nasammāro,",
  "anya-e1004n0:349": "Napiti nacabandhavā;",
  "anya-e1004n0:350": "Nacavijjāgamo koci,",
  "anya-e1004n0:351": "Natattha vasatī vasse.",
  "anya-e1004n0:352": "70.",
  "anya-e1004n0:353": "Dhanavā gaṇako rājā,",
  "anya-e1004n0:354": "Nadī vejjā imejanā;",
  "anya-e1004n0:355": "Yattha pañca navijjante,",
  "anya-e1004n0:356": "Na tatthavasatī vase.",
  "anya-e1004n0:357": "71.",
  "anya-e1004n0:358": "Asanaṁ bhayamantānaṁ,",
  "anya-e1004n0:359": "Maccānaṁ maraṇaṁ bhayaṁ;",
  "anya-e1004n0:360": "Uttamānantusabbesaṁ,",
  "anya-e1004n0:361": "Avamānaṁ paraṁ bhayaṁ.",
  "anya-e1004n0:362": "72.",
  "anya-e1004n0:363": "Amānanā yattha siyā,",
  "anya-e1004n0:364": "Santānaṁ avamānanā;",
  "anya-e1004n0:365": "Hīnasamānanāvāpi,",
  "anya-e1004n0:366": "Na tatthavasati vase.",
  "anya-e1004n0:367": "73.",
  "anya-e1004n0:368": "Yatthālasoca dakkhoca,",
  "anya-e1004n0:369": "Sūro bhīruca pūjito;",
  "anya-e1004n0:370": "Na santā tattha vassanti,",
  "anya-e1004n0:371": "Avisesakarena ko.",
  "anya-e1004n0:372": "74.",
  "anya-e1004n0:373": "Dukkho nivāso sambādhe,",
  "anya-e1004n0:374": "Ṭhāne asucisaṅkaṭe;",
  "anya-e1004n0:375": "Tato rimhipiyānante,",
  "anya-e1004n0:376": "Tatoca akataññunaṁ.",
  "anya-e1004n0:377": "75.",
  "anya-e1004n0:378": "Siṅgīnaṁ pañcahatthena,",
  "anya-e1004n0:379": "Satena vājinaṁ caje;",
  "anya-e1004n0:380": "Hatthīnantu sahassena,",
  "anya-e1004n0:381": "Desacāgena dujjane.",
  "anya-e1004n0:382": "76.",
  "anya-e1004n0:383": "Caje ekaṁ kulassatthaṁ,",
  "anya-e1004n0:384": "Gāmassatthaṁ kulaṁ caje;",
  "anya-e1004n0:385": "Gāmaṁ janapadassatthaṁ;",
  "anya-e1004n0:386": "Attatthaṁ pathavī caje.",
  "anya-e1004n0:387": "77.",
  "anya-e1004n0:388": "Calatyekenapādena,",
  "anya-e1004n0:389": "Tiṭṭhatyekena paṇḍito;",
  "anya-e1004n0:390": "Asamikhyaparaṁṭhānaṁ,",
  "anya-e1004n0:391": "Pubbamāyanaṁnacaje.",
  "anya-e1004n0:392": "78.",
  "anya-e1004n0:393": "Ṭhānabhaṭṭhā nasobhanti,",
  "anya-e1004n0:394": "Dantākesāna khānarā;",
  "anya-e1004n0:395": "Matimā itiviññāya,",
  "anya-e1004n0:396": "Saṭhānaṁ nacaje lahuṁ.",
  "anya-e1004n0:397": "79.",
  "anya-e1004n0:398": "Caje dhanaṁ aṅgavarassahetu,",
  "anya-e1004n0:399": "Aṅgaṁ caje jivitaṁ rakkhamāno;",
  "anya-e1004n0:400": "Aṅgaṁ dhanaṁ jivitañcāpi sabbaṁ,",
  "anya-e1004n0:401": "Caje naro dhamma manussaranto.",
  "anya-e1004n0:402": "80.",
  "anya-e1004n0:403": "Sotaṁ suteneva na kuṇḍalena,",
  "anya-e1004n0:404": "Dānena pāṇī natu kaṅkaṇena;",
  "anya-e1004n0:405": "Ābhāti kāyo purisuttamassa,",
  "anya-e1004n0:406": "Paropakārena na candanena."
}