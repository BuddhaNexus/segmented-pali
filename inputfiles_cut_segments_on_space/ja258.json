{
  "ja258:0.1": "Jātaka ",
  "ja258:0.2": "Tikanipāta ",
  "ja258:0.3": "Saṅkappavagga ",
  "ja258:0.4": "8. Mandhātujātaka ",
  "ja258:1.1": "“Yāvatā candimasūriyā, ",
  "ja258:1.2": "Pariharanti disā bhanti virocanā; ",
  "ja258:1.3": "Sabbeva dāsā mandhātu, ",
  "ja258:1.4": "Ye pāṇā pathavissitā. ",
  "ja258:2.1": "Na kahāpaṇavassena, ",
  "ja258:2.2": "titti kāmesu vijjati; ",
  "ja258:2.3": "Appassādā dukhā kāmā, ",
  "ja258:2.4": "iti viññāya paṇḍito. ",
  "ja258:3.1": "Api dibbesu kāmesu, ",
  "ja258:3.2": "ratiṁ so nādhigacchati; ",
  "ja258:3.3": "Taṇhakkhayarato hoti, ",
  "ja258:3.4": "sammāsambuddhasāvako”ti. ",
  "ja258:4.1": "Mandhātujātakaṁ aṭṭhamaṁ. "
}