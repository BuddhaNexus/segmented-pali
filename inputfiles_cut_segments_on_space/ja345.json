{
  "ja345:0.1": "Jātaka ",
  "ja345:0.2": "Catukkanipāta ",
  "ja345:0.3": "Cūḷakuṇālavagga ",
  "ja345:0.4": "5. Gajakumbhajātaka ",
  "ja345:1.1": "“Vanaṁ yadaggi dahati, ",
  "ja345:1.2": "pāvako kaṇhavattanī; ",
  "ja345:1.3": "Kathaṁ karosi pacalaka, ",
  "ja345:1.4": "evaṁ dandhaparakkamo”. ",
  "ja345:2.1": "“Bahūni rukkhachiddāni, ",
  "ja345:2.2": "pathabyā vivarāni ca; ",
  "ja345:2.3": "Tāni ce nābhisambhoma, ",
  "ja345:2.4": "hoti no kālapariyāyo”. ",
  "ja345:3.1": "“Yo dandhakāle tarati, ",
  "ja345:3.2": "taraṇīye ca dandhati; ",
  "ja345:3.3": "Sukkhapaṇṇaṁva akkamma, ",
  "ja345:3.4": "atthaṁ bhañjati attano. ",
  "ja345:4.1": "Yo dandhakāle dandheti, ",
  "ja345:4.2": "taraṇīye ca tārayi; ",
  "ja345:4.3": "Sasīva rattiṁ vibhajaṁ, ",
  "ja345:4.4": "tassattho paripūratī”ti. ",
  "ja345:5.1": "Gajakumbhajātakaṁ pañcamaṁ. "
}