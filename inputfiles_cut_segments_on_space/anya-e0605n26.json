{
  "anya-e0605n26:0": "Navaguṇadīpanīgāthā",
  "anya-e0605n26:1": "177.",
  "anya-e0605n26:2": "Evaṁ hi buddhattamupāgato so,",
  "anya-e0605n26:3": "Desesi dhammaṁ sanarāmarānaṁ;",
  "anya-e0605n26:4": "Nānānayehībhisamesi satte,",
  "anya-e0605n26:5": "Tasmā hi jhāto tibhavesu nātho.",
  "anya-e0605n26:6": "178.",
  "anya-e0605n26:7": "Addhā laddhā dhammālokaṁ,",
  "anya-e0605n26:8": "Diṭṭhā pattā ñātā saccaṁ;",
  "anya-e0605n26:9": "Tiññārāgādosamohā,",
  "anya-e0605n26:10": "Thomesuṁ te devā brahmā.",
  "anya-e0605n26:11": "179.",
  "anya-e0605n26:12": "Munirājavaro nararājavaro,",
  "anya-e0605n26:13": "Dividevavaro sucibrahmavaro;",
  "anya-e0605n26:14": "Sakapāpaharo parapāpaharo,",
  "anya-e0605n26:15": "Sakavuḍḍhikaro paravuḍḍhikaro.",
  "anya-e0605n26:16": "180.",
  "anya-e0605n26:17": "Sanarāmarubrahmagaṇebhi rutā,",
  "anya-e0605n26:18": "Arahādiguṇā vipulā vimalā;",
  "anya-e0605n26:19": "Navadhā vasudhāgagaṇe,",
  "anya-e0605n26:20": "Sakale tidive tibhave visaṭā.",
  "anya-e0605n26:21": "181.",
  "anya-e0605n26:22": "Ye pissa te bhagavato ca acintiyādī,",
  "anya-e0605n26:23": "Suddhātisuddhatarabuddhaguṇā hi sabbe;",
  "anya-e0605n26:24": "Saṅkhepato navavidhesu padesu khittā,",
  "anya-e0605n26:25": "Vakkhāmi dāni arahādiguṇe ahaṁ pi.",
  "anya-e0605n26:26": "182.",
  "anya-e0605n26:27": "Yo cīdha jāto arahaṁ nirāso,",
  "anya-e0605n26:28": "Sammābhisambuddhasamantacakkhu;",
  "anya-e0605n26:29": "Sampannavijjācaraṇoghatiṇṇo,",
  "anya-e0605n26:30": "Sammāgato so sugato gato va.",
  "anya-e0605n26:31": "183.",
  "anya-e0605n26:32": "Avedi so lokamimaṁ parañca,",
  "anya-e0605n26:33": "Amuttaro sārathidammasatte;",
  "anya-e0605n26:34": "Sadevakānaṁ varasatthukiccaṁ,",
  "anya-e0605n26:35": "Akāsi buddho bhagavā visuddho."
}