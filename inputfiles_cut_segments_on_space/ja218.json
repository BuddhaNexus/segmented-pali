{
  "ja218:0.1": "Jātaka ",
  "ja218:0.2": "Dukanipāta ",
  "ja218:0.3": "Bīraṇathambhavagga ",
  "ja218:0.4": "8. Kūṭavāṇijajātaka ",
  "ja218:1.1": "“Saṭhassa sāṭheyyamidaṁ sucintitaṁ, ",
  "ja218:1.2": "Paccoḍḍitaṁ paṭikūṭassa kūṭaṁ; ",
  "ja218:1.3": "Phālañce khādeyyuṁ mūsikā, ",
  "ja218:1.4": "Kasmā kumāraṁ kulalā na hareyyuṁ. ",
  "ja218:2.1": "Kūṭassa hi santi kūṭakūṭā, ",
  "ja218:2.2": "Bhavati cāpi nikatino nikatyā; ",
  "ja218:2.3": "Dehi puttanaṭṭha phālanaṭṭhassa phālaṁ, ",
  "ja218:2.4": "Mā te puttamahāsi phālanaṭṭho”ti. ",
  "ja218:3.1": "Kūṭavāṇijajātakaṁ aṭṭhamaṁ. "
}