{
  "ja310:0.1": "Jātaka ",
  "ja310:0.2": "Catukkanipāta ",
  "ja310:0.3": "Kāliṅgavagga ",
  "ja310:0.4": "10. Seyyajātaka ",
  "ja310:1.1": "“Sasamuddapariyāyaṁ, ",
  "ja310:1.2": "mahiṁ sāgarakuṇḍalaṁ; ",
  "ja310:1.3": "Na icche saha nindāya, ",
  "ja310:1.4": "evaṁ seyya vijānahi. ",
  "ja310:2.1": "Dhiratthu taṁ yasalābhaṁ, ",
  "ja310:2.2": "dhanalābhañca brāhmaṇa; ",
  "ja310:2.3": "Yā vutti vinipātena, ",
  "ja310:2.4": "adhammacaraṇena vā. ",
  "ja310:3.1": "Api ce pattamādāya, ",
  "ja310:3.2": "anagāro paribbaje; ",
  "ja310:3.3": "Sāyeva jīvikā seyyo, ",
  "ja310:3.4": "yā cādhammena esanā. ",
  "ja310:4.1": "Api ce pattamādāya, ",
  "ja310:4.2": "anagāro paribbaje; ",
  "ja310:4.3": "Aññaṁ ahiṁsayaṁ loke, ",
  "ja310:4.4": "api rajjena taṁ varan”ti. ",
  "ja310:5.1": "Seyyajātakaṁ dasamaṁ. ",
  "ja310:5.2": "Kāliṅgavaggo paṭhamo. ",
  "ja310:6.1": "Tassuddānaṁ ",
  "ja310:7.1": "Vivarañca adeyya samiddhavaraṁ, ",
  "ja310:7.2": "Atha daddara pāpamahātiraho; ",
  "ja310:7.3": "Atha koli palāsavarañca kara, ",
  "ja310:7.4": "Carimaṁ sasamuddavarena dasāti. "
}