{
  "ja216:0.1_0": "Jātaka",
  "ja216:0.2_0": "Dukanipāta",
  "ja216:0.3_0": "Bīraṇathambhavagga",
  "ja216:0.4_0": "6. Macchajātaka",
  "ja216:1.1_0": "“Na māyamaggi tapati,",
  "ja216:1.2_0": "na sūlo sādhutacchito;",
  "ja216:1.3_0": "Yañca maṁ maññate macchī,",
  "ja216:1.4_0": "aññaṁ so ratiyā gato.",
  "ja216:2.1_0": "So maṁ dahati rāgaggi,",
  "ja216:2.2_0": "cittaṁ cūpatapeti maṁ;",
  "ja216:2.3_0": "Jālino muñcathāyirā maṁ,",
  "ja216:2.4_0": "na kāme haññate kvacī”ti.",
  "ja216:3.1_0": "Macchajātakaṁ chaṭṭhaṁ."
}