{
  "atk-s0402a18:0_0": "4. Rāgapeyyālaṁ",
  "atk-s0402a18:1_0": "231.Rāgassa, bhikkhave, abhiññāyāti pañcakāmaguṇikarāgassa abhijānanatthaṁ paccakkhakaraṇatthaṁ.",
  "atk-s0402a18:1_1": "Pariññāyāti parijānanatthaṁ.",
  "atk-s0402a18:1_2": "Parikkhayāyāti parikkhayagamanatthaṁ.",
  "atk-s0402a18:1_3": "Pahānāyāti pajahanatthaṁ.",
  "atk-s0402a18:1_4": "Khayāya vayāyāti khayavayagamanatthaṁ.",
  "atk-s0402a18:1_5": "Virāgāyāti virajjanatthaṁ.",
  "atk-s0402a18:1_6": "Nirodhāyāti nirujjhanatthaṁ.",
  "atk-s0402a18:1_7": "Cāgāyāti cajanatthaṁ.",
  "atk-s0402a18:1_8": "Paṭinissaggāyāti paṭinissajjanatthaṁ.",
  "atk-s0402a18:2_0": "232-246.Thambhassāti kodhamānavasena thaddhabhāvassa.",
  "atk-s0402a18:2_1": "Sārabbhassāti kāraṇuttariyalakkhaṇassa sārabbhassa.",
  "atk-s0402a18:2_2": "Mānassāti navavidhamānassa.",
  "atk-s0402a18:2_3": "Atimānassāti atikkamitvā maññanamānassa.",
  "atk-s0402a18:2_4": "Madassāti majjanākāramadassa.",
  "atk-s0402a18:2_5": "Pamādassāti sativippavāsassa, pañcasu kāmaguṇesu cittavossaggassa.",
  "atk-s0402a18:2_6": "Sesaṁ sabbattha uttānatthamevāti.",
  "atk-s0402a18:3_0": "Rāgapeyyālaṁ niṭṭhitaṁ.",
  "atk-s0402a18:4_0": "Manorathapūraṇiyā aṅguttaranikāya-aṭṭhakathāya",
  "atk-s0402a18:5_0": "Dukanipātassa saṁvaṇṇanā niṭṭhitā.",
  "atk-s0402a18:6_0": " Namo tassa bhagavato arahato sammāsambuddhassa",
  "atk-s0402a18:7_0": "Aṅguttaranikāye",
  "atk-s0402a18:8_0": "Tikanipāta-aṭṭhakathā",
  "atk-s0402a18:9_0": "1. Paṭhamapaṇṇāsakaṁ"
}