{
  "tha-ap65:0.1_0": "Therāpadāna",
  "tha-ap65:0.2_0": "Sakacintaniyavagga",
  "tha-ap65:0.3_0": "3. Paccāgamaniyattheraapadāna",
  "tha-ap65:1.1_0": "“Sindhuyā nadiyā tīre,",
  "tha-ap65:1.2_0": "cakkavāko ahaṁ tadā;",
  "tha-ap65:1.3_0": "Suddhasevālabhakkhohaṁ,",
  "tha-ap65:1.4_0": "pāpesu ca susaññato.",
  "tha-ap65:2.1_0": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap65:2.2_0": "gacchantaṁ anilañjase;",
  "tha-ap65:2.3_0": "Tuṇḍena sālaṁ paggayha,",
  "tha-ap65:2.4_0": "vipassissābhiropayiṁ.",
  "tha-ap65:3.1_0": "Yassa saddhā tathāgate,",
  "tha-ap65:3.2_0": "acalā supatiṭṭhitā;",
  "tha-ap65:3.3_0": "Tena cittappasādena,",
  "tha-ap65:3.4_0": "duggatiṁ so na gacchati.",
  "tha-ap65:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap65:4.2_0": "buddhaseṭṭhassa santike;",
  "tha-ap65:4.3_0": "Vihaṅgamena santena,",
  "tha-ap65:4.4_0": "subījaṁ ropitaṁ mayā.",
  "tha-ap65:5.1_0": "Ekanavutito kappe,",
  "tha-ap65:5.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap65:5.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap65:5.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap65:6.1_0": "Sucārudassanā nāma,",
  "tha-ap65:6.2_0": "aṭṭhete ekanāmakā;",
  "tha-ap65:6.3_0": "Kappe sattarase āsuṁ,",
  "tha-ap65:6.4_0": "cakkavattī mahabbalā.",
  "tha-ap65:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap65:7.2_0": "…pe…",
  "tha-ap65:7.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap65:8.1_0": "Itthaṁ sudaṁ āyasmā paccāgamaniyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap65:9.1_0": "Paccāgamaniyattherassāpadānaṁ tatiyaṁ."
}