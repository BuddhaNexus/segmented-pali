{
  "tha-ap277:0.1_0": "Therāpadāna",
  "tha-ap277:0.2_0": "Suvaṇṇabibbohanavagga",
  "tha-ap277:0.3_0": "5 Ekañjalikattheraapadāna",
  "tha-ap277:1.1_0": "“Udumbare vasantassa,",
  "tha-ap277:1.2_0": "niyate paṇṇasanthare;",
  "tha-ap277:1.3_0": "Vutthokāso mayā dinno,",
  "tha-ap277:1.4_0": "samaṇassa mahesino.",
  "tha-ap277:2.1_0": "Tissassa dvipadindassa,",
  "tha-ap277:2.2_0": "lokanāthassa tādino;",
  "tha-ap277:2.3_0": "Añjaliṁ paggahetvāna,",
  "tha-ap277:2.4_0": "santhariṁ pupphasantharaṁ.",
  "tha-ap277:3.1_0": "Dvenavute ito kappe,",
  "tha-ap277:3.2_0": "yaṁ kariṁ pupphasantharaṁ;",
  "tha-ap277:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap277:3.4_0": "santharassa idaṁ phalaṁ.",
  "tha-ap277:4.1_0": "Ito cuddasakappamhi,",
  "tha-ap277:4.2_0": "ahosiṁ manujādhipo;",
  "tha-ap277:4.3_0": "Ekaañjaliko nāma,",
  "tha-ap277:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap277:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap277:5.2_0": "…pe…",
  "tha-ap277:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap277:6.1_0": "Itthaṁ sudaṁ āyasmā ekañjaliko thero imā gāthāyo abhāsitthāti.",
  "tha-ap277:7.1_0": "Ekañjalikattherassāpadānaṁ pañcamaṁ."
}