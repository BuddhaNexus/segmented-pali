{
  "tha-ap551:0.1_0": "Therāpadāna",
  "tha-ap551:0.2_0": "Bhaddiyavagga",
  "tha-ap551:0.3_0": "9. Vanavacchattheraapadāna",
  "tha-ap551:1.1_0": "“Imamhi bhaddake kappe,",
  "tha-ap551:1.2_0": "brahmabandhu mahāyaso;",
  "tha-ap551:1.3_0": "Kassapo nāma gottena,",
  "tha-ap551:1.4_0": "uppajji vadataṁ varo.",
  "tha-ap551:2.1_0": "Tadāhaṁ pabbajitvāna,",
  "tha-ap551:2.2_0": "tassa buddhassa sāsane;",
  "tha-ap551:2.3_0": "Yāvajīvaṁ caritvāna,",
  "tha-ap551:2.4_0": "brahmacāraṁ tato cuto.",
  "tha-ap551:3.1_0": "Tena kammena sukatena,",
  "tha-ap551:3.2_0": "cetanāpaṇidhīhi ca;",
  "tha-ap551:3.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "tha-ap551:3.4_0": "tāvatiṁsamagacchahaṁ.",
  "tha-ap551:4.1_0": "Tato cuto araññamhi,",
  "tha-ap551:4.2_0": "kapoto āsahaṁ tahiṁ;",
  "tha-ap551:4.3_0": "Vasate guṇasampanno,",
  "tha-ap551:4.4_0": "bhikkhu jhānarato sadā.",
  "tha-ap551:5.1_0": "Mettacitto kāruṇiko,",
  "tha-ap551:5.2_0": "sadā pamuditānano;",
  "tha-ap551:5.3_0": "Upekkhako mahāvīro,",
  "tha-ap551:5.4_0": "appamaññāsu kovido.",
  "tha-ap551:6.1_0": "Vinīvaraṇasaṅkappe,",
  "tha-ap551:6.2_0": "sabbasattahitāsaye;",
  "tha-ap551:6.3_0": "Visaṭṭho nacirenāsiṁ,",
  "tha-ap551:6.4_0": "tasmiṁ sugatasāvake.",
  "tha-ap551:7.1_0": "Upecca pādamūlamhi,",
  "tha-ap551:7.2_0": "nisinnassa tadāssame;",
  "tha-ap551:7.3_0": "Kadāci sāmisaṁ deti,",
  "tha-ap551:7.4_0": "dhammaṁ desesi cekadā.",
  "tha-ap551:8.1_0": "Tadā vipulapemena,",
  "tha-ap551:8.2_0": "upāsitvā jinatrajaṁ;",
  "tha-ap551:8.3_0": "Tato cuto gato saggaṁ,",
  "tha-ap551:8.4_0": "pavāso sagharaṁ yathā.",
  "tha-ap551:9.1_0": "Saggā cuto manussesu,",
  "tha-ap551:9.2_0": "nibbatto puññakammunā;",
  "tha-ap551:9.3_0": "Agāraṁ chaḍḍayitvāna,",
  "tha-ap551:9.4_0": "pabbajiṁ bahuso ahaṁ.",
  "tha-ap551:10.1_0": "Samaṇo tāpaso vippo,",
  "tha-ap551:10.2_0": "paribbajo tathevahaṁ;",
  "tha-ap551:10.3_0": "Hutvā vasiṁ araññamhi,",
  "tha-ap551:10.4_0": "anekasataso ahaṁ.",
  "tha-ap551:11.1_0": "Pacchime ca bhave dāni,",
  "tha-ap551:11.2_0": "ramme kapilavatthave;",
  "tha-ap551:11.3_0": "Vacchagotto dijo tassa,",
  "tha-ap551:11.4_0": "jāyāya ahamokkamiṁ.",
  "tha-ap551:12.1_0": "Mātu me dohaḷo āsi,",
  "tha-ap551:12.2_0": "tirokucchigatassa me;",
  "tha-ap551:12.3_0": "Jāyamānasamīpamhi,",
  "tha-ap551:12.4_0": "vanavāsāya nicchayo.",
  "tha-ap551:13.1_0": "Tato me ajanī mātā,",
  "tha-ap551:13.2_0": "ramaṇīye vanantare;",
  "tha-ap551:13.3_0": "Gabbhato nikkhamantaṁ maṁ,",
  "tha-ap551:13.4_0": "kāsāyena paṭiggahuṁ.",
  "tha-ap551:14.1_0": "Tato kumāro siddhattho,",
  "tha-ap551:14.2_0": "jāto sakyakuladdhajo;",
  "tha-ap551:14.3_0": "Tassa mitto piyo āsiṁ,",
  "tha-ap551:14.4_0": "saṁvisaṭṭho sumāniyo.",
  "tha-ap551:15.1_0": "Sattasārebhinikkhante,",
  "tha-ap551:15.2_0": "ohāya vipulaṁ yasaṁ;",
  "tha-ap551:15.3_0": "Ahampi pabbajitvāna,",
  "tha-ap551:15.4_0": "himavantamupāgamiṁ.",
  "tha-ap551:16.1_0": "Vanālayaṁ bhāvanīyaṁ,",
  "tha-ap551:16.2_0": "kassapaṁ dhutavādikaṁ;",
  "tha-ap551:16.3_0": "Disvā sutvā jinuppādaṁ,",
  "tha-ap551:16.4_0": "upesiṁ narasārathiṁ.",
  "tha-ap551:17.1_0": "So me dhammamadesesi,",
  "tha-ap551:17.2_0": "sabbatthaṁ sampakāsayaṁ;",
  "tha-ap551:17.3_0": "Tatohaṁ pabbajitvāna,",
  "tha-ap551:17.4_0": "vanameva punāgamaṁ.",
  "tha-ap551:18.1_0": "Tatthāppamatto viharaṁ,",
  "tha-ap551:18.2_0": "chaḷabhiññā aphassayiṁ;",
  "tha-ap551:18.3_0": "Aho suladdhalābhomhi,",
  "tha-ap551:18.4_0": "sumittenānukampito.",
  "tha-ap551:19.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap551:19.2_0": "…pe…",
  "tha-ap551:19.3_0": "viharāmi anāsavo.",
  "tha-ap551:20.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap551:20.2_0": "…pe…",
  "tha-ap551:20.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap551:21.1_0": "Paṭisambhidā catasso,",
  "tha-ap551:21.2_0": "…pe…",
  "tha-ap551:21.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap551:22.1_0": "Itthaṁ sudaṁ āyasmā vanavaccho thero imā gāthāyo abhāsitthāti.",
  "tha-ap551:23.1_0": "Vanavacchattherassāpadānaṁ navamaṁ."
}