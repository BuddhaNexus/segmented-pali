{
  "anya-e0605n26:0_0": "Navaguṇadīpanīgāthā",
  "anya-e0605n26:1_0": "177.",
  "anya-e0605n26:2_0": " Evaṁ hi buddhattamupāgato so,",
  "anya-e0605n26:3_0": "Desesi dhammaṁ sanarāmarānaṁ;",
  "anya-e0605n26:4_0": "Nānānayehībhisamesi satte,",
  "anya-e0605n26:5_0": "Tasmā hi jhāto tibhavesu nātho.",
  "anya-e0605n26:6_0": "178.",
  "anya-e0605n26:7_0": " Addhā laddhā dhammālokaṁ,",
  "anya-e0605n26:8_0": "Diṭṭhā pattā ñātā saccaṁ;",
  "anya-e0605n26:9_0": "Tiññārāgādosamohā,",
  "anya-e0605n26:10_0": "Thomesuṁ te devā brahmā.",
  "anya-e0605n26:11_0": "179.",
  "anya-e0605n26:12_0": " Munirājavaro nararājavaro,",
  "anya-e0605n26:13_0": "Dividevavaro sucibrahmavaro;",
  "anya-e0605n26:14_0": "Sakapāpaharo parapāpaharo,",
  "anya-e0605n26:15_0": "Sakavuḍḍhikaro paravuḍḍhikaro.",
  "anya-e0605n26:16_0": "180.",
  "anya-e0605n26:17_0": " Sanarāmarubrahmagaṇebhi rutā,",
  "anya-e0605n26:18_0": "Arahādiguṇā vipulā vimalā;",
  "anya-e0605n26:19_0": "Navadhā vasudhāgagaṇe,",
  "anya-e0605n26:20_0": "Sakale tidive tibhave visaṭā.",
  "anya-e0605n26:21_0": "181.",
  "anya-e0605n26:22_0": " Ye pissa te bhagavato ca acintiyādī,",
  "anya-e0605n26:23_0": "Suddhātisuddhatarabuddhaguṇā hi sabbe;",
  "anya-e0605n26:24_0": "Saṅkhepato navavidhesu padesu khittā,",
  "anya-e0605n26:25_0": "Vakkhāmi dāni arahādiguṇe ahaṁ pi.",
  "anya-e0605n26:26_0": "182.",
  "anya-e0605n26:27_0": " Yo cīdha jāto arahaṁ nirāso,",
  "anya-e0605n26:28_0": "Sammābhisambuddhasamantacakkhu;",
  "anya-e0605n26:29_0": "Sampannavijjācaraṇoghatiṇṇo,",
  "anya-e0605n26:30_0": "Sammāgato so sugato gato va.",
  "anya-e0605n26:31_0": "183.",
  "anya-e0605n26:32_0": " Avedi so lokamimaṁ parañca,",
  "anya-e0605n26:33_0": "Amuttaro sārathidammasatte;",
  "anya-e0605n26:34_0": "Sadevakānaṁ varasatthukiccaṁ,",
  "anya-e0605n26:35_0": "Akāsi buddho bhagavā visuddho."
}