{
  "mil7.1:0.1_0": "Milindapañha",
  "mil7.1:0.2_0": "7 Opammakathāpañha",
  "mil7.1:0.3_0": "Mātikā",
  "mil7.1:1.1_0": "“Bhante nāgasena, katihaṅgehi samannāgato bhikkhu arahattaṁ sacchikarotī”ti?",
  "mil7.1:2.1_0": "“Idha, mahārāja, arahattaṁ sacchikātukāmena bhikkhunā—",
  "mil7.1:3.1_0": "Gadrabhassa ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:4.1_0": "Kukkuṭassa pañca aṅgāni gahetabbāni.",
  "mil7.1:5.1_0": "Kalandakassa ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:6.1_0": "Dīpiniyā ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:7.1_0": "Dīpikassa dve aṅgāni gahetabbāni.",
  "mil7.1:8.1_0": "Kummassa pañca aṅgāni gahetabbāni.",
  "mil7.1:9.1_0": "Vaṁsassa ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:10.1_0": "Cāpassa ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:11.1_0": "Vāyasassa dve aṅgāni gahetabbāni.",
  "mil7.1:12.1_0": "Makkaṭassa dve aṅgāni gahetabbāni.",
  "mil7.1:13.1_0": "Gadrabhavaggo paṭhamo.",
  "mil7.1:14.1_0": "Lābulatāya ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:15.1_0": "Padumassa tīṇi aṅgāni gahetabbāni.",
  "mil7.1:16.1_0": "Bījassa dve aṅgāni gahetabbāni.",
  "mil7.1:17.1_0": "Sālakalyāṇikāya ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:18.1_0": "Nāvāya tīṇi aṅgāni gahetabbāni.",
  "mil7.1:19.1_0": "Nāvālagganakassa dve aṅgāni gahetabbāni.",
  "mil7.1:20.1_0": "Kūpassa ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:21.1_0": "Niyāmakassa tīṇi aṅgāni gahetabbāni.",
  "mil7.1:22.1_0": "Kammakārassa ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:23.1_0": "Samuddassa pañca aṅgāni gahetabbāni.",
  "mil7.1:24.1_0": "Samuddavaggo dutiyo.",
  "mil7.1:25.1_0": "Pathaviyā pañca aṅgāni gahetabbāni.",
  "mil7.1:26.1_0": "Āpassa pañca aṅgāni gahetabbāni.",
  "mil7.1:27.1_0": "Tejassa pañca aṅgāni gahetabbāni.",
  "mil7.1:28.1_0": "Vāyussa pañca aṅgāni gahetabbāni.",
  "mil7.1:29.1_0": "Pabbatassa pañca aṅgāni gahetabbāni.",
  "mil7.1:30.1_0": "Ākāsassa pañca aṅgāni gahetabbāni.",
  "mil7.1:31.1_0": "Candassa pañca aṅgāni gahetabbāni.",
  "mil7.1:32.1_0": "Sūriyassa satta aṅgāni gahetabbāni.",
  "mil7.1:33.1_0": "Sakkassa tīṇi aṅgāni gahetabbāni.",
  "mil7.1:34.1_0": "Cakkavattissa cattāri aṅgāni gahetabbāni.",
  "mil7.1:35.1_0": "Pathavīvaggo tatiyo.",
  "mil7.1:36.1_0": "Upacikāya ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:37.1_0": "Biḷārassa dve aṅgāni gahetabbāni.",
  "mil7.1:38.1_0": "Undūrassa ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:39.1_0": "Vicchikassa ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:40.1_0": "Nakulassa ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:41.1_0": "Jarasiṅgālassa dve aṅgāni gahetabbāni.",
  "mil7.1:42.1_0": "Migassa tīṇi aṅgāni gahetabbāni.",
  "mil7.1:43.1_0": "Gorūpassa cattāri aṅgāni gahetabbāni.",
  "mil7.1:44.1_0": "Varāhassa dve aṅgāni gahetabbāni.",
  "mil7.1:45.1_0": "Hatthissa pañca aṅgāni gahetabbāni.",
  "mil7.1:46.1_0": "Upacikāvaggo catuttho.",
  "mil7.1:47.1_0": "Sīhassa satta aṅgāni gahetabbāni.",
  "mil7.1:48.1_0": "Cakkavākassa tīṇi aṅgāni gahetabbāni.",
  "mil7.1:49.1_0": "Peṇāhikāya dve aṅgāni gahetabbāni.",
  "mil7.1:50.1_0": "Gharakapotassa ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:51.1_0": "Ulūkassa dve aṅgāni gahetabbāni.",
  "mil7.1:52.1_0": "Satapattassa ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:53.1_0": "Vaggulissa dve aṅgāni gahetabbāni.",
  "mil7.1:54.1_0": "Jalūkāya ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:55.1_0": "Sappassa tīṇi aṅgāni gahetabbāni.",
  "mil7.1:56.1_0": "Ajagarassa ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:57.1_0": "Sīhavaggo pañcamo.",
  "mil7.1:58.1_0": "Panthamakkaṭakassa ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:59.1_0": "Thanasitadārakassa ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:60.1_0": "Cittakadharakummassa ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:61.1_0": "Pavanassa pañca aṅgāni gahetabbāni.",
  "mil7.1:62.1_0": "Rukkhassa tīṇi aṅgāni gahetabbāni.",
  "mil7.1:63.1_0": "Meghassa pañca aṅgāni gahetabbāni.",
  "mil7.1:64.1_0": "Maṇiratanassa tīṇi aṅgāni gahetabbāni.",
  "mil7.1:65.1_0": "Māgavikassa cattāri aṅgāni gahetabbāni.",
  "mil7.1:66.1_0": "Bāḷisikassa dve aṅgāni gahetabbāni.",
  "mil7.1:67.1_0": "Tacchakassa dve aṅgāni gahetabbāni.",
  "mil7.1:68.1_0": "Makkaṭavaggo chaṭṭho.",
  "mil7.1:69.1_0": "Kumbhassa ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:70.1_0": "Kāḷāyasassa dve aṅgāni gahetabbāni.",
  "mil7.1:71.1_0": "Chattassa tīṇi aṅgāni gahetabbāni.",
  "mil7.1:72.1_0": "Khettassa tīṇi aṅgāni gahetabbāni.",
  "mil7.1:73.1_0": "Agadassa dve aṅgāni gahetabbāni.",
  "mil7.1:74.1_0": "Bhojanassa tīṇi aṅgāni gahetabbāni.",
  "mil7.1:75.1_0": "Issāsassa cattāri aṅgāni gahetabbāni.",
  "mil7.1:76.1_0": "Kumbhavaggo sattamo.",
  "mil7.1:77.1_0": "Rañño cattāri aṅgāni gahetabbāni.",
  "mil7.1:78.1_0": "Dovārikassa dve aṅgāni gahetabbāni.",
  "mil7.1:79.1_0": "Nisadāya ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:80.1_0": "Padīpassa dve aṅgāni gahetabbāni.",
  "mil7.1:81.1_0": "Mayūrassa dve aṅgāni gahetabbāni.",
  "mil7.1:82.1_0": "Turaṅgassa dve aṅgāni gahetabbāni.",
  "mil7.1:83.1_0": "Soṇḍikassa dve aṅgāni gahetabbāni.",
  "mil7.1:84.1_0": "Indakhīlassa dve aṅgāni gahetabbāni.",
  "mil7.1:85.1_0": "Tulāya ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:86.1_0": "Khaggassa dve aṅgāni gahetabbāni.",
  "mil7.1:87.1_0": "Macchassa dve aṅgāni gahetabbāni.",
  "mil7.1:88.1_0": "Iṇaggāhakassa ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:89.1_0": "Byādhitassa dve aṅgāni gahetabbāni.",
  "mil7.1:90.1_0": "Matassa dve aṅgāni gahetabbāni.",
  "mil7.1:91.1_0": "Nadiyā dve aṅgāni gahetabbāni.",
  "mil7.1:92.1_0": "Usabhassa ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:93.1_0": "Maggassa dve aṅgāni gahetabbāni.",
  "mil7.1:94.1_0": "Suṅkasāyikassa ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:95.1_0": "Corassa tīṇi aṅgāni gahetabbāni.",
  "mil7.1:96.1_0": "Sakuṇagghiyā ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:97.1_0": "Sunakhassa ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:98.1_0": "Tikicchakassa tīṇi aṅgāni gahetabbāni.",
  "mil7.1:99.1_0": "Gabbhiniyā dve aṅgāni gahetabbāni.",
  "mil7.1:100.1_0": "Camariyā ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:101.1_0": "Kikiyā dve aṅgāni gahetabbāni.",
  "mil7.1:102.1_0": "Kapotikāya tīṇi aṅgāni gahetabbāni.",
  "mil7.1:103.1_0": "Ekanayanassa dve aṅgāni gahetabbāni.",
  "mil7.1:104.1_0": "Kassakassa tīṇi aṅgāni gahetabbāni.",
  "mil7.1:105.1_0": "Jambukasiṅgāliyā ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:106.1_0": "Caṅgavārakassa dve aṅgāni gahetabbāni.",
  "mil7.1:107.1_0": "Dabbiyā ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:108.1_0": "Iṇasādhakassa tīṇi aṅgāni gahetabbāni.",
  "mil7.1:109.1_0": "Anuvicinakassa ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:110.1_0": "Sārathissa dve aṅgāni gahetabbāni.",
  "mil7.1:111.1_0": "Bhojakassa dve aṅgāni gahetabbāni.",
  "mil7.1:112.1_0": "Tunnavāyassa ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:113.1_0": "Nāvikassa ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.1:114.1_0": "Bhamarassa dve aṅgāni gahetabbānī”ti.",
  "mil7.1:115.1_0": "Mātikā niṭṭhitā."
}