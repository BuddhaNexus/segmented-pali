{
  "thag10.4:0.1_0": "Theragāthā",
  "thag10.4:0.2_0": "Dasakanipāta",
  "thag10.4:0.3_0": "Paṭhamavagga",
  "thag10.4:0.4_0": "10.4. Cūḷapanthakattheragāthā",
  "thag10.4:1.1_0": "“Dandhā mayhaṁ gatī āsi,",
  "thag10.4:1.2_0": "paribhūto pure ahaṁ;",
  "thag10.4:1.3_0": "Bhātā ca maṁ paṇāmesi,",
  "thag10.4:1.4_0": "‘gaccha dāni tuvaṁ gharaṁ’.",
  "thag10.4:2.1_0": "Sohaṁ paṇāmito santo,",
  "thag10.4:2.2_0": "saṅghārāmassa koṭṭhake;",
  "thag10.4:2.3_0": "Dummano tattha aṭṭhāsiṁ,",
  "thag10.4:2.4_0": "sāsanasmiṁ apekkhavā.",
  "thag10.4:3.1_0": "Bhagavā tattha āgacchi,",
  "thag10.4:3.2_0": "sīsaṁ mayhaṁ parāmasi;",
  "thag10.4:3.3_0": "Bāhāya maṁ gahetvāna,",
  "thag10.4:3.4_0": "saṅghārāmaṁ pavesayi.",
  "thag10.4:4.1_0": "Anukampāya me satthā,",
  "thag10.4:4.2_0": "pādāsi pādapuñchaniṁ;",
  "thag10.4:4.3_0": "‘Etaṁ suddhaṁ adhiṭṭhehi,",
  "thag10.4:4.4_0": "ekamantaṁ svadhiṭṭhitaṁ’.",
  "thag10.4:5.1_0": "Tassāhaṁ vacanaṁ sutvā,",
  "thag10.4:5.2_0": "vihāsiṁ sāsane rato;",
  "thag10.4:5.3_0": "Samādhiṁ paṭipādesiṁ,",
  "thag10.4:5.4_0": "uttamatthassa pattiyā.",
  "thag10.4:6.1_0": "Pubbenivāsaṁ jānāmi,",
  "thag10.4:6.2_0": "dibbacakkhu visodhitaṁ;",
  "thag10.4:6.3_0": "Tisso vijjā anuppattā,",
  "thag10.4:6.4_0": "kataṁ buddhassa sāsanaṁ.",
  "thag10.4:7.1_0": "Sahassakkhattumattānaṁ,",
  "thag10.4:7.2_0": "nimminitvāna panthako;",
  "thag10.4:7.3_0": "Nisīdambavane ramme,",
  "thag10.4:7.4_0": "yāva kālappavedanā.",
  "thag10.4:8.1_0": "Tato me satthā pāhesi,",
  "thag10.4:8.2_0": "dūtaṁ kālappavedakaṁ;",
  "thag10.4:8.3_0": "Paveditamhi kālamhi,",
  "thag10.4:8.4_0": "vehāsādupasaṅkamiṁ.",
  "thag10.4:9.1_0": "Vanditvā satthuno pāde,",
  "thag10.4:9.2_0": "ekamantaṁ nisīdahaṁ;",
  "thag10.4:9.3_0": "Nisinnaṁ maṁ viditvāna,",
  "thag10.4:9.4_0": "atha satthā paṭiggahi.",
  "thag10.4:10.1_0": "Āyāgo sabbalokassa,",
  "thag10.4:10.2_0": "āhutīnaṁ paṭiggaho;",
  "thag10.4:10.3_0": "Puññakkhettaṁ manussānaṁ,",
  "thag10.4:10.4_0": "paṭiggaṇhittha dakkhiṇan”ti.",
  "thag10.4:10.5_0": "… Cūḷapanthako thero …."
}