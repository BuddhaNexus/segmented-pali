{
  "tha-ap259:0.1_0": "Therāpadāna",
  "tha-ap259:0.2_0": "Thomakavagga",
  "tha-ap259:0.3_0": "7. Mañjaripūjakattheraapadāna",
  "tha-ap259:1.1_0": "“Mañjarikaṁ karitvāna,",
  "tha-ap259:1.2_0": "rathiyaṁ paṭipajjahaṁ;",
  "tha-ap259:1.3_0": "Addasaṁ samaṇānaggaṁ,",
  "tha-ap259:1.4_0": "bhikkhusaṅghapurakkhataṁ.",
  "tha-ap259:2.1_0": "Pasannacitto sumano,",
  "tha-ap259:2.2_0": "paramāya ca pītiyā;",
  "tha-ap259:2.3_0": "Ubho hatthehi paggayha,",
  "tha-ap259:2.4_0": "buddhassa abhiropayiṁ.",
  "tha-ap259:3.1_0": "Dvenavute ito kappe,",
  "tha-ap259:3.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap259:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap259:3.4_0": "pupphapūjāyidaṁ phalaṁ.",
  "tha-ap259:4.1_0": "Ito tesattatikappe,",
  "tha-ap259:4.2_0": "eko āsiṁ mahīpati;",
  "tha-ap259:4.3_0": "Jotiyo nāma nāmena,",
  "tha-ap259:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap259:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap259:5.2_0": "…pe…",
  "tha-ap259:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap259:6.1_0": "Itthaṁ sudaṁ āyasmā mañjaripūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap259:7.1_0": "Mañjaripūjakattherassāpadānaṁ sattamaṁ."
}