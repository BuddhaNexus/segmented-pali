{
  "pli-tv-bu-vb-sk20:0.1_0": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk20:0.2_0": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk20:0.3_0": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk20:0.4_0": "Ujjagghikavagga",
  "pli-tv-bu-vb-sk20:0.5_0": "20. Dutiyasīsappacālakasikkhāpada",
  "pli-tv-bu-vb-sk20:1.1_0": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk20:1.2_0": "Tena kho pana samayena chabbaggiyā bhikkhū sīsappacālakaṁ antaraghare nisīdanti sīsaṁ olambentā …pe….",
  "pli-tv-bu-vb-sk20:1.3.1_0": "“Na sīsappacālakaṁ antaraghare nisīdissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk20:1.4_0": "Na sīsappacālakaṁ antaraghare nisīditabbaṁ.",
  "pli-tv-bu-vb-sk20:1.5_0": "Sīsaṁ paggahetvā nisīditabbaṁ.",
  "pli-tv-bu-vb-sk20:1.6_0": "Yo anādariyaṁ paṭicca sīsappacālakaṁ antaraghare nisīdati sīsaṁ olambento, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk20:1.7.1_0": "Anāpatti—",
  "pli-tv-bu-vb-sk20:1.8_0": "asañcicca,",
  "pli-tv-bu-vb-sk20:1.9_0": "assatiyā,",
  "pli-tv-bu-vb-sk20:1.10_0": "ajānantassa,",
  "pli-tv-bu-vb-sk20:1.11_0": "gilānassa,",
  "pli-tv-bu-vb-sk20:1.12_0": "vāsūpagatassa,",
  "pli-tv-bu-vb-sk20:1.13_0": "āpadāsu,",
  "pli-tv-bu-vb-sk20:1.14_0": "ummattakassa,",
  "pli-tv-bu-vb-sk20:1.15_0": "ādikammikassāti.",
  "pli-tv-bu-vb-sk20:1.16_0": "Dasamasikkhāpadaṁ niṭṭhitaṁ.",
  "pli-tv-bu-vb-sk20:1.17_0": "Ujjagghikavaggo dutiyo."
}