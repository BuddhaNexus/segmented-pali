{
  "tha-ap232:0.1_0": "Therāpadāna",
  "tha-ap232:0.2_0": "Ālambaṇadāyakavagga",
  "tha-ap232:0.3_0": "10 Ekavandaniyattheraapadāna",
  "tha-ap232:1.1_0": "“Usabhaṁ pavaraṁ vīraṁ,",
  "tha-ap232:1.2_0": "vessabhuṁ vijitāvinaṁ;",
  "tha-ap232:1.3_0": "Pasannacitto sumano,",
  "tha-ap232:1.4_0": "buddhaseṭṭhamavandahaṁ.",
  "tha-ap232:2.1_0": "Ekattiṁse ito kappe,",
  "tha-ap232:2.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap232:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap232:2.4_0": "vandanāya idaṁ phalaṁ.",
  "tha-ap232:3.1_0": "Catuvīsatikappamhi,",
  "tha-ap232:3.2_0": "vikatānandanāmako;",
  "tha-ap232:3.3_0": "Sattaratanasampanno,",
  "tha-ap232:3.4_0": "cakkavattī mahabbalo.",
  "tha-ap232:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap232:4.2_0": "…pe…",
  "tha-ap232:4.3_0": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap232:5.1_0": "Itthaṁ sudaṁ āyasmā ekavandaniyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap232:6.1_0": "Ekavandaniyattherassāpadānaṁ dasamaṁ.",
  "tha-ap232:7.1_0": "Ālambaṇadāyakavaggo tevīsatimo.",
  "tha-ap232:8.1_0": "Tassuddānaṁ",
  "tha-ap232:9.1_0": "Ālambaṇañca ajinaṁ,",
  "tha-ap232:9.2_0": "maṁsadārakkhadāyako;",
  "tha-ap232:9.3_0": "Abyādhi aṅkolaṁ soṇṇaṁ,",
  "tha-ap232:9.4_0": "miñjaāveḷavandanaṁ;",
  "tha-ap232:9.5_0": "Pañcapaññāsa gāthāyo,",
  "tha-ap232:9.6_0": "gaṇitā atthadassibhi."
}