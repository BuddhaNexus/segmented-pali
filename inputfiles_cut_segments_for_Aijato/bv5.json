{
  "bv5:0.1_0": "Buddhavaṁsa",
  "bv5:0.2_0": "5. Maṅgalabuddhavaṁsa",
  "bv5:1.1_0": "“Koṇḍaññassa aparena,",
  "bv5:1.2_0": "maṅgalo nāma nāyako;",
  "bv5:1.3_0": "Tamaṁ loke nihantvāna,",
  "bv5:1.4_0": "dhammokkamabhidhārayi.",
  "bv5:2.1_0": "Atulāsi pabhā tassa,",
  "bv5:2.2_0": "jinehaññehi uttariṁ;",
  "bv5:2.3_0": "Candasūriyapabhaṁ hantvā,",
  "bv5:2.4_0": "dasasahassī virocati.",
  "bv5:3.1_0": "Sopi buddho pakāsesi,",
  "bv5:3.2_0": "caturo saccavaruttame;",
  "bv5:3.3_0": "Te te saccarasaṁ pītvā,",
  "bv5:3.4_0": "vinodenti mahātamaṁ.",
  "bv5:4.1_0": "Patvāna bodhimatulaṁ,",
  "bv5:4.2_0": "paṭhame dhammadesane;",
  "bv5:4.3_0": "Koṭisatasahassānaṁ,",
  "bv5:4.4_0": "dhammābhisamayo ahu.",
  "bv5:5.1_0": "Surindadevabhavane,",
  "bv5:5.2_0": "buddho dhammamadesayi;",
  "bv5:5.3_0": "Tadā koṭisahassānaṁ,",
  "bv5:5.4_0": "dutiyo samayo ahu.",
  "bv5:6.1_0": "Yadā sunando cakkavattī,",
  "bv5:6.2_0": "sambuddhaṁ upasaṅkami;",
  "bv5:6.3_0": "Tadā āhani sambuddho,",
  "bv5:6.4_0": "dhammabheriṁ varuttamaṁ.",
  "bv5:7.1_0": "Sunandassānucarā janatā,",
  "bv5:7.2_0": "Tadāsuṁ navutikoṭiyo;",
  "bv5:7.3_0": "Sabbepi te niravasesā,",
  "bv5:7.4_0": "Ahesuṁ ehi bhikkhukā.",
  "bv5:8.1_0": "Sannipātā tayo āsuṁ,",
  "bv5:8.2_0": "maṅgalassa mahesino;",
  "bv5:8.3_0": "Koṭisatasahassānaṁ,",
  "bv5:8.4_0": "paṭhamo āsi samāgamo.",
  "bv5:9.1_0": "Dutiyo koṭisatasahassānaṁ,",
  "bv5:9.2_0": "Tatiyo navutikoṭinaṁ;",
  "bv5:9.3_0": "Khīṇāsavānaṁ vimalānaṁ,",
  "bv5:9.4_0": "Tadā āsi samāgamo.",
  "bv5:10.1_0": "Ahaṁ tena samayena,",
  "bv5:10.2_0": "surucī nāma brāhmaṇo;",
  "bv5:10.3_0": "Ajjhāyako mantadharo,",
  "bv5:10.4_0": "tiṇṇaṁ vedāna pāragū.",
  "bv5:11.1_0": "Tamahaṁ upasaṅkamma,",
  "bv5:11.2_0": "Saraṇaṁ gantvāna satthuno;",
  "bv5:11.3_0": "Sambuddhappamukhaṁ saṅghaṁ,",
  "bv5:11.4_0": "Gandhamālena pūjayiṁ;",
  "bv5:11.5_0": "Pūjetvā gandhamālena,",
  "bv5:11.6_0": "Gavapānena tappayiṁ.",
  "bv5:12.1_0": "Sopi maṁ buddho byākāsi,",
  "bv5:12.2_0": "maṅgalo dvipaduttamo;",
  "bv5:12.3_0": "‘Aparimeyyito kappe,",
  "bv5:12.4_0": "ayaṁ buddho bhavissati.",
  "bv5:13.1_0": "Padhānaṁ padahitvāna,",
  "bv5:13.2_0": "…pe…",
  "bv5:13.3_0": "hessāma sammukhā imaṁ’.",
  "bv5:14.1_0": "Tassāpi vacanaṁ sutvā,",
  "bv5:14.2_0": "bhiyyo cittaṁ pasādayiṁ;",
  "bv5:14.3_0": "Uttariṁ vatamadhiṭṭhāsiṁ,",
  "bv5:14.4_0": "dasapāramipūriyā.",
  "bv5:15.1_0": "Tadā pītimanubrūhanto,",
  "bv5:15.2_0": "sambodhivarapattiyā;",
  "bv5:15.3_0": "Buddhe datvāna maṁ gehaṁ,",
  "bv5:15.4_0": "pabbajiṁ tassa santike.",
  "bv5:16.1_0": "Suttantaṁ vinayañcāpi,",
  "bv5:16.2_0": "navaṅgaṁ satthusāsanaṁ;",
  "bv5:16.3_0": "Sabbaṁ pariyāpuṇitvā,",
  "bv5:16.4_0": "sobhayiṁ jinasāsanaṁ.",
  "bv5:17.1_0": "Tatthappamatto viharanto,",
  "bv5:17.2_0": "brahmaṁ bhāvetva bhāvanaṁ;",
  "bv5:17.3_0": "Abhiññāpāramiṁ gantvā,",
  "bv5:17.4_0": "brahmalokamagacchahaṁ.",
  "bv5:18.1_0": "Uttaraṁ nāma nagaraṁ,",
  "bv5:18.2_0": "uttaro nāma khattiyo;",
  "bv5:18.3_0": "Uttarā nāma janikā,",
  "bv5:18.4_0": "maṅgalassa mahesino.",
  "bv5:19.1_0": "Navavassasahassāni,",
  "bv5:19.2_0": "agāraṁ ajjha so vasi;",
  "bv5:19.3_0": "Yasavā sucimā sirīmā,",
  "bv5:19.4_0": "tayo pāsādamuttamā.",
  "bv5:20.1_0": "Samatiṁsasahassāni,",
  "bv5:20.2_0": "nāriyo samalaṅkatā;",
  "bv5:20.3_0": "Yasavatī nāma nārī,",
  "bv5:20.4_0": "sīvalo nāma atrajo.",
  "bv5:21.1_0": "Nimitte caturo disvā,",
  "bv5:21.2_0": "assayānena nikkhami;",
  "bv5:21.3_0": "Anūnaaṭṭhamāsāni,",
  "bv5:21.4_0": "padhānaṁ padahī jino.",
  "bv5:22.1_0": "Brahmunā yācito santo,",
  "bv5:22.2_0": "maṅgalo nāma nāyako;",
  "bv5:22.3_0": "Vatti cakkaṁ mahāvīro,",
  "bv5:22.4_0": "vane sirīvaruttame.",
  "bv5:23.1_0": "Sudevo dhammaseno ca,",
  "bv5:23.2_0": "ahesuṁ aggasāvakā;",
  "bv5:23.3_0": "Pālito nāmupaṭṭhāko,",
  "bv5:23.4_0": "maṅgalassa mahesino.",
  "bv5:24.1_0": "Sīvalā ca asokā ca,",
  "bv5:24.2_0": "ahesuṁ aggasāvikā;",
  "bv5:24.3_0": "Bodhi tassa bhagavato,",
  "bv5:24.4_0": "nāgarukkhoti vuccati.",
  "bv5:25.1_0": "Nando ceva visākho ca,",
  "bv5:25.2_0": "ahesuṁ aggupaṭṭhakā;",
  "bv5:25.3_0": "Anulā ceva sutanā ca,",
  "bv5:25.4_0": "ahesuṁ aggupaṭṭhikā.",
  "bv5:26.1_0": "Aṭṭhāsīti ratanāni,",
  "bv5:26.2_0": "accuggato mahāmuni;",
  "bv5:26.3_0": "Tato niddhāvatī raṁsī,",
  "bv5:26.4_0": "anekasatasahassiyo.",
  "bv5:27.1_0": "Navutivassasahassāni,",
  "bv5:27.2_0": "āyu vijjati tāvade;",
  "bv5:27.3_0": "Tāvatā tiṭṭhamāno so,",
  "bv5:27.4_0": "tāresi janataṁ bahuṁ.",
  "bv5:28.1_0": "Yathāpi sāgare ūmī,",
  "bv5:28.2_0": "na sakkā tā gaṇetuye;",
  "bv5:28.3_0": "Tatheva sāvakā tassa,",
  "bv5:28.4_0": "na sakkā te gaṇetuye.",
  "bv5:29.1_0": "Yāva aṭṭhāsi sambuddho,",
  "bv5:29.2_0": "maṅgalo lokanāyako;",
  "bv5:29.3_0": "Na tassa sāsane atthi,",
  "bv5:29.4_0": "sakilesamaraṇaṁ tadā.",
  "bv5:30.1_0": "Dhammokkaṁ dhārayitvāna,",
  "bv5:30.2_0": "santāretvā mahājanaṁ;",
  "bv5:30.3_0": "Jalitvā dhūmaketūva,",
  "bv5:30.4_0": "nibbuto so mahāyaso.",
  "bv5:31.1_0": "Saṅkhārānaṁ sabhāvatthaṁ,",
  "bv5:31.2_0": "dassayitvā sadevake;",
  "bv5:31.3_0": "Jalitvā aggikkhandhova,",
  "bv5:31.4_0": "sūriyo atthaṅgato yathā.",
  "bv5:32.1_0": "Uyyāne vassare nāma,",
  "bv5:32.2_0": "Buddho nibbāyi maṅgalo;",
  "bv5:32.3_0": "Tatthevassa jinathūpo,",
  "bv5:32.4_0": "Tiṁsayojanamuggato”ti. ",
  "bv5:33.1_0": "Maṅgalassa bhagavato vaṁso tatiyo."
}