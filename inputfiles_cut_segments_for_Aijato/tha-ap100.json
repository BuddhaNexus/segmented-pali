{
  "tha-ap100:0.1_0": "Therāpadāna",
  "tha-ap100:0.2_0": "Sudhāvagga",
  "tha-ap100:0.3_0": "8. Senāsanadāyakattheraapadāna",
  "tha-ap100:1.1_0": "“Siddhatthassa bhagavato,",
  "tha-ap100:1.2_0": "adāsiṁ paṇṇasantharaṁ;",
  "tha-ap100:1.3_0": "Samantā upahārañca,",
  "tha-ap100:1.4_0": "kusumaṁ okiriṁ ahaṁ.",
  "tha-ap100:2.1_0": "Pāsādevaṁ guṇaṁ rammaṁ,",
  "tha-ap100:2.2_0": "Anubhomi mahārahaṁ;",
  "tha-ap100:2.3_0": "Mahagghāni ca pupphāni,",
  "tha-ap100:2.4_0": "Sayanebhisavanti me.",
  "tha-ap100:3.1_0": "Sayanehaṁ tuvaṭṭāmi,",
  "tha-ap100:3.2_0": "vicitte pupphasanthate;",
  "tha-ap100:3.3_0": "Pupphavuṭṭhi ca sayane,",
  "tha-ap100:3.4_0": "abhivassati tāvade.",
  "tha-ap100:4.1_0": "Catunnavutito kappe,",
  "tha-ap100:4.2_0": "adāsiṁ paṇṇasantharaṁ;",
  "tha-ap100:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap100:4.4_0": "santharassa idaṁ phalaṁ.",
  "tha-ap100:5.1_0": "Tiṇasantharakā nāma,",
  "tha-ap100:5.2_0": "sattete cakkavattino;",
  "tha-ap100:5.3_0": "Ito te pañcame kappe,",
  "tha-ap100:5.4_0": "uppajjiṁsu janādhipā.",
  "tha-ap100:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap100:6.2_0": "…pe…",
  "tha-ap100:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap100:7.1_0": "Itthaṁ sudaṁ āyasmā senāsanadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap100:8.1_0": "Senāsanadāyakattherassāpadānaṁ aṭṭhamaṁ."
}