{
  "ja392:0.1_0": "Jātaka",
  "ja392:0.2_0": "Chakkanipāta",
  "ja392:0.3_0": "Kharaputtavagga",
  "ja392:0.4_0": "7. Siṅghapupphajātaka",
  "ja392:1.1_0": "“Yametaṁ vārijaṁ pupphaṁ,",
  "ja392:1.2_0": "Adinnaṁ upasiṅghasi;",
  "ja392:1.3_0": "Ekaṅgametaṁ theyyānaṁ,",
  "ja392:1.4_0": "Gandhathenosi mārisa”.",
  "ja392:2.1_0": "“Na harāmi na bhañjāmi,",
  "ja392:2.2_0": "ārā siṅghāmi vārijaṁ;",
  "ja392:2.3_0": "Atha kena nu vaṇṇena,",
  "ja392:2.4_0": "gandhathenoti vuccati.",
  "ja392:3.1_0": "Yoyaṁ bhisāni khaṇati,",
  "ja392:3.2_0": "puṇḍarīkāni bhañjati;",
  "ja392:3.3_0": "Evaṁ ākiṇṇakammanto,",
  "ja392:3.4_0": "kasmā eso na vuccati”.",
  "ja392:4.1_0": "“Ākiṇṇaluddo puriso,",
  "ja392:4.2_0": "dhāticelaṁva makkhito;",
  "ja392:4.3_0": "Tasmiṁ me vacanaṁ natthi,",
  "ja392:4.4_0": "tañcārahāmi vattave.",
  "ja392:5.1_0": "Anaṅgaṇassa posassa,",
  "ja392:5.2_0": "niccaṁ sucigavesino;",
  "ja392:5.3_0": "Vālaggamattaṁ pāpassa,",
  "ja392:5.4_0": "abbhāmattaṁva khāyati”.",
  "ja392:6.1_0": "“Addhā maṁ yakkha jānāsi,",
  "ja392:6.2_0": "atho maṁ anukampasi;",
  "ja392:6.3_0": "Punapi yakkha vajjāsi,",
  "ja392:6.4_0": "yadā passasi edisaṁ”.",
  "ja392:7.1_0": "“Neva taṁ upajīvāmi,",
  "ja392:7.2_0": "napi te bhatakāmhase;",
  "ja392:7.3_0": "Tvameva bhikkhu jāneyya,",
  "ja392:7.4_0": "yena gaccheyya suggatin”ti.",
  "ja392:8.1_0": "Siṅghapupphajātakaṁ sattamaṁ."
}