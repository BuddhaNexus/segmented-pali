{
  "snp4.5:0.1_0": "Sutta Nipāta 4.5",
  "snp4.5:0.2_0": "Paramaṭṭhakasutta",
  "snp4.5:1.1_0": "Paramanti diṭṭhīsu paribbasāno,",
  "snp4.5:1.2_0": "Yaduttari kurute jantu loke;",
  "snp4.5:1.3_0": "Hīnāti aññe tato sabbamāha,",
  "snp4.5:1.4_0": "Tasmā vivādāni avītivatto.",
  "snp4.5:2.1_0": "Yadattanī passati ānisaṁsaṁ,",
  "snp4.5:2.2_0": "Diṭṭhe sute sīlavate mute vā;",
  "snp4.5:2.3_0": "Tadeva so tattha samuggahāya,",
  "snp4.5:2.4_0": "Nihīnato passati sabbamaññaṁ.",
  "snp4.5:3.1_0": "Taṁ vāpi ganthaṁ kusalā vadanti,",
  "snp4.5:3.2_0": "Yaṁ nissito passati hīnamaññaṁ;",
  "snp4.5:3.3_0": "Tasmā hi diṭṭhaṁ va sutaṁ mutaṁ vā,",
  "snp4.5:3.4_0": "Sīlabbataṁ bhikkhu na nissayeyya.",
  "snp4.5:4.1_0": "Diṭṭhimpi lokasmiṁ na kappayeyya,",
  "snp4.5:4.2_0": "Ñāṇena vā sīlavatena vāpi;",
  "snp4.5:4.3_0": "Samoti attānamanūpaneyya,",
  "snp4.5:4.4_0": "Hīno na maññetha visesi vāpi.",
  "snp4.5:5.1_0": "Attaṁ pahāya anupādiyāno,",
  "snp4.5:5.2_0": "Ñāṇepi so nissayaṁ no karoti;",
  "snp4.5:5.3_0": "Sa ve viyattesu na vaggasārī,",
  "snp4.5:5.4_0": "Diṭṭhimpi so na pacceti kiñci.",
  "snp4.5:6.1_0": "Yassūbhayante paṇidhīdha natthi,",
  "snp4.5:6.2_0": "Bhavābhavāya idha vā huraṁ vā;",
  "snp4.5:6.3_0": "Nivesanā tassa na santi keci,",
  "snp4.5:6.4_0": "Dhammesu niccheyya samuggahītaṁ.",
  "snp4.5:7.1_0": "Tassīdha diṭṭhe va sute mute vā,",
  "snp4.5:7.2_0": "Pakappitā natthi aṇūpi saññā;",
  "snp4.5:7.3_0": "Taṁ brāhmaṇaṁ diṭṭhimanādiyānaṁ,",
  "snp4.5:7.4_0": "Kenīdha lokasmiṁ vikappayeyya.",
  "snp4.5:8.1_0": "Na kappayanti na purekkharonti,",
  "snp4.5:8.2_0": "Dhammāpi tesaṁ na paṭicchitāse;",
  "snp4.5:8.3_0": "Na brāhmaṇo sīlavatena neyyo,",
  "snp4.5:8.4_0": "Pāraṅgato na pacceti tādīti.",
  "snp4.5:8.5_0": "Paramaṭṭhakasuttaṁ pañcamaṁ."
}