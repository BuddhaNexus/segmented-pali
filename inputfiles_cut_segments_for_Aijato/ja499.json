{
  "ja499:0.1_0": "Jātaka",
  "ja499:0.2_0": "Vīsatinipāta",
  "ja499:0.3_0": "Mātaṅgavagga",
  "ja499:0.4_0": "3. Sivijātaka",
  "ja499:1.1_0": "“Dūre apassaṁ therova,",
  "ja499:1.2_0": "cakkhuṁ yācitumāgato;",
  "ja499:1.3_0": "Ekanettā bhavissāma,",
  "ja499:1.4_0": "cakkhuṁ me dehi yācito”.",
  "ja499:2.1_0": "“Kenānusiṭṭho idha māgatosi,",
  "ja499:2.2_0": "Vanibbaka cakkhupathāni yācituṁ;",
  "ja499:2.3_0": "Suduccajaṁ yācasi uttamaṅgaṁ,",
  "ja499:2.4_0": "Yamāhu nettaṁ purisena duccajaṁ”.",
  "ja499:3.1_0": "“Yamāhu devesu sujampatīti,",
  "ja499:3.2_0": "Maghavāti naṁ āhu manussaloke;",
  "ja499:3.3_0": "Tenānusiṭṭho idha māgatosmi,",
  "ja499:3.4_0": "Vanibbako cakkhupathāni yācituṁ.",
  "ja499:4.1_0": "Vanibbato mayha vaniṁ anuttaraṁ,",
  "ja499:4.2_0": "Dadāhi te cakkhupathāni yācito;",
  "ja499:4.3_0": "Dadāhi me cakkhupathaṁ anuttaraṁ,",
  "ja499:4.4_0": "Yamāhu nettaṁ purisena duccajaṁ”.",
  "ja499:5.1_0": "“Yena atthena āgacchi,",
  "ja499:5.2_0": "yamatthamabhipatthayaṁ;",
  "ja499:5.3_0": "Te te ijjhantu saṅkappā,",
  "ja499:5.4_0": "labha cakkhūni brāhmaṇa.",
  "ja499:6.1_0": "Ekaṁ te yācamānassa,",
  "ja499:6.2_0": "Ubhayāni dadāmahaṁ;",
  "ja499:6.3_0": "Sa cakkhumā gaccha janassa pekkhato,",
  "ja499:6.4_0": "Yadicchase tvaṁ tadate samijjhatu”.",
  "ja499:7.1_0": "“Mā no deva adā cakkhuṁ,",
  "ja499:7.2_0": "mā no sabbe parākari;",
  "ja499:7.3_0": "Dhanaṁ dehi mahārāja,",
  "ja499:7.4_0": "muttā veḷuriyā bahū.",
  "ja499:8.1_0": "Yutte deva rathe dehi,",
  "ja499:8.2_0": "ājānīye calaṅkate;",
  "ja499:8.3_0": "Nāge dehi mahārāja,",
  "ja499:8.4_0": "hemakappanavāsase.",
  "ja499:9.1_0": "Yathā taṁ sivayo sabbe,",
  "ja499:9.2_0": "sayoggā sarathā sadā;",
  "ja499:9.3_0": "Samantā parikireyyuṁ,",
  "ja499:9.4_0": "evaṁ dehi rathesabha”.",
  "ja499:10.1_0": "“Yo ve dassanti vatvāna,",
  "ja499:10.2_0": "adāne kurute mano;",
  "ja499:10.3_0": "Bhūmyaṁ so patitaṁ pāsaṁ,",
  "ja499:10.4_0": "gīvāyaṁ paṭimuñcati.",
  "ja499:11.1_0": "Yo ve dassanti vatvāna,",
  "ja499:11.2_0": "adāne kurute mano;",
  "ja499:11.3_0": "Pāpā pāpataro hoti,",
  "ja499:11.4_0": "sampatto yamasādhanaṁ.",
  "ja499:12.1_0": "Yañhi yāce tañhi dade,",
  "ja499:12.2_0": "yaṁ na yāce na taṁ dade;",
  "ja499:12.3_0": "Svāhaṁ tameva dassāmi,",
  "ja499:12.4_0": "yaṁ maṁ yācati brāhmaṇo”.",
  "ja499:13.1_0": "“Āyuṁ nu vaṇṇaṁ nu sukhaṁ balaṁ nu,",
  "ja499:13.2_0": "Kiṁ patthayāno nu janinda desi;",
  "ja499:13.3_0": "Kathañhi rājā sivinaṁ anuttaro,",
  "ja499:13.4_0": "Cakkhūni dajjā paralokahetu”.",
  "ja499:14.1_0": "“Na vāhametaṁ yasasā dadāmi,",
  "ja499:14.2_0": "Na puttamicche na dhanaṁ na raṭṭhaṁ;",
  "ja499:14.3_0": "Satañca dhammo carito purāṇo,",
  "ja499:14.4_0": "Icceva dāne ramate mano mama”.",
  "ja499:14.5_0": "(…)",
  "ja499:15.1_0": "“Sakhā ca mitto ca mamāsi sīvika,",
  "ja499:15.2_0": "Susikkhito sādhu karohi me vaco;",
  "ja499:15.3_0": "Uddharitvā cakkhūni mamaṁ jigīsato,",
  "ja499:15.4_0": "Hatthesu ṭhapehi vanibbakassa”.",
  "ja499:16.1_0": "Codito sivirājena,",
  "ja499:16.2_0": "sīviko vacanaṅkaro;",
  "ja499:16.3_0": "Rañño cakkhūnuddharitvā,",
  "ja499:16.4_0": "brāhmaṇassūpanāmayi;",
  "ja499:16.5_0": "Sacakkhu brāhmaṇo āsi,",
  "ja499:16.6_0": "andho rājā upāvisi.",
  "ja499:17.1_0": "Tato so katipāhassa,",
  "ja499:17.2_0": "uparūḷhesu cakkhusu;",
  "ja499:17.3_0": "Sūtaṁ āmantayī rājā,",
  "ja499:17.4_0": "sivīnaṁ raṭṭhavaḍḍhano.",
  "ja499:18.1_0": "“Yojehi sārathi yānaṁ,",
  "ja499:18.2_0": "yuttañca paṭivedaya;",
  "ja499:18.3_0": "Uyyānabhūmiṁ gacchāma,",
  "ja499:18.4_0": "pokkharañño vanāni ca”.",
  "ja499:19.1_0": "So ca pokkharaṇītīre,",
  "ja499:19.2_0": "pallaṅkena upāvisi;",
  "ja499:19.3_0": "Tassa sakko pāturahu,",
  "ja499:19.4_0": "devarājā sujampati.",
  "ja499:20.1_0": "“Sakkohamasmi devindo,",
  "ja499:20.2_0": "āgatosmi tavantike;",
  "ja499:20.3_0": "Varaṁ varassu rājīsi,",
  "ja499:20.4_0": "yaṁ kiñci manasicchasi”.",
  "ja499:21.1_0": "“Pahūtaṁ me dhanaṁ sakka,",
  "ja499:21.2_0": "balaṁ koso canappako;",
  "ja499:21.3_0": "Andhassa me sato dāni,",
  "ja499:21.4_0": "maraṇaññeva ruccati”.",
  "ja499:22.1_0": "“Yāni saccāni dvipadinda,",
  "ja499:22.2_0": "tāni bhāsassu khattiya;",
  "ja499:22.3_0": "Saccaṁ te bhaṇamānassa,",
  "ja499:22.4_0": "puna cakkhu bhavissati”.",
  "ja499:23.1_0": "“Ye maṁ yācitumāyanti,",
  "ja499:23.2_0": "nānāgottā vanibbakā;",
  "ja499:23.3_0": "Yopi maṁ yācate tattha,",
  "ja499:23.4_0": "sopi me manaso piyo;",
  "ja499:23.5_0": "Etena saccavajjena,",
  "ja499:23.6_0": "cakkhu me upapajjatha.",
  "ja499:24.1_0": "Yaṁ maṁ so yācituṁ āgā,",
  "ja499:24.2_0": "dehi cakkhunti brāhmaṇo;",
  "ja499:24.3_0": "Tassa cakkhūni pādāsiṁ,",
  "ja499:24.4_0": "brāhmaṇassa vanibbato.",
  "ja499:25.1_0": "Bhiyyo maṁ āvisī pīti,",
  "ja499:25.2_0": "somanassañcanappakaṁ;",
  "ja499:25.3_0": "Etena saccavajjena,",
  "ja499:25.4_0": "dutiyaṁ me upapajjatha”.",
  "ja499:26.1_0": "“Dhammena bhāsitā gāthā,",
  "ja499:26.2_0": "sivīnaṁ raṭṭhavaḍḍhana;",
  "ja499:26.3_0": "Etāni tava nettāni,",
  "ja499:26.4_0": "dibbāni paṭidissare.",
  "ja499:27.1_0": "Tirokuṭṭaṁ tiroselaṁ,",
  "ja499:27.2_0": "samatiggayha pabbataṁ;",
  "ja499:27.3_0": "Samantā yojanasataṁ,",
  "ja499:27.4_0": "dassanaṁ anubhontu te”.",
  "ja499:28.1_0": "“Ko nīdha vittaṁ na dadeyya yācito,",
  "ja499:28.2_0": "Api visiṭṭhaṁ supiyampi attano;",
  "ja499:28.3_0": "Tadiṅgha sabbe sivayo samāgatā,",
  "ja499:28.4_0": "Dibbāni nettāni mamajja passatha.",
  "ja499:29.1_0": "Tirokuṭṭaṁ tiroselaṁ,",
  "ja499:29.2_0": "samatiggayha pabbataṁ;",
  "ja499:29.3_0": "Samantā yojanasataṁ,",
  "ja499:29.4_0": "dassanaṁ anubhonti me.",
  "ja499:30.1_0": "Na cāgamattā paramatthi kiñci,",
  "ja499:30.2_0": "Maccānaṁ idha jīvite;",
  "ja499:30.3_0": "Datvāna mānusaṁ cakkhuṁ,",
  "ja499:30.4_0": "Laddhaṁ me cakkhuṁ amānusaṁ.",
  "ja499:31.1_0": "Etampi disvā sivayo,",
  "ja499:31.2_0": "Detha dānāni bhuñjatha;",
  "ja499:31.3_0": "Datvā ca bhutvā ca yathānubhāvaṁ,",
  "ja499:31.4_0": "Aninditā saggamupetha ṭhānan”ti.",
  "ja499:32.1_0": "Sivijātakaṁ tatiyaṁ."
}