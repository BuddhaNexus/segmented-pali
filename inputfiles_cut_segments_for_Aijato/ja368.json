{
  "ja368:0.1_0": "Jātaka",
  "ja368:0.2_0": "Pañcakanipāta",
  "ja368:0.3_0": "Vaṇṇārohavagga",
  "ja368:0.4_0": "8. Tacasārajātaka",
  "ja368:1.1_0": "“Amittahatthatthagatā,",
  "ja368:1.2_0": "tacasārasamappitā;",
  "ja368:1.3_0": "Pasannamukhavaṇṇāttha,",
  "ja368:1.4_0": "kasmā tumhe na socatha”.",
  "ja368:2.1_0": "“Na socanāya paridevanāya,",
  "ja368:2.2_0": "Atthova labbho api appakopi;",
  "ja368:2.3_0": "Socantamenaṁ dukhitaṁ viditvā,",
  "ja368:2.4_0": "Paccatthikā attamanā bhavanti.",
  "ja368:3.1_0": "Yato ca kho paṇḍito āpadāsu,",
  "ja368:3.2_0": "Na vedhatī atthavinicchayaññū;",
  "ja368:3.3_0": "Paccatthikāssa dukhitā bhavanti,",
  "ja368:3.4_0": "Disvā mukhaṁ avikāraṁ purāṇaṁ.",
  "ja368:4.1_0": "Jappena mantena subhāsitena,",
  "ja368:4.2_0": "Anuppadānena paveṇiyā vā;",
  "ja368:4.3_0": "Yathā yathā yattha labhetha atthaṁ,",
  "ja368:4.4_0": "Tathā tathā tattha parakkameyya.",
  "ja368:5.1_0": "Yato ca jāneyya alabbhaneyyo,",
  "ja368:5.2_0": "Mayā va aññena vā esa attho;",
  "ja368:5.3_0": "Asocamāno adhivāsayeyya,",
  "ja368:5.4_0": "Kammaṁ daḷhaṁ kinti karomi dānī”ti.",
  "ja368:6.1_0": "Tacasārajātakaṁ aṭṭhamaṁ."
}