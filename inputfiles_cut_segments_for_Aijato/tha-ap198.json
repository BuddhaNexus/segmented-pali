{
  "tha-ap198:0.1_0": "Therāpadāna",
  "tha-ap198:0.2_0": "Tamālapupphiyavagga",
  "tha-ap198:0.3_0": "6. Kisalayapūjakattheraapadāna",
  "tha-ap198:1.1_0": "“Nagare dvāravatiyā,",
  "tha-ap198:1.2_0": "mālāvaccho mamaṁ ahu;",
  "tha-ap198:1.3_0": "Udapāno ca tattheva,",
  "tha-ap198:1.4_0": "pādapānaṁ virohano.",
  "tha-ap198:2.1_0": "Sabalena upatthaddho,",
  "tha-ap198:2.2_0": "siddhattho aparājito;",
  "tha-ap198:2.3_0": "Mamānukampamāno so,",
  "tha-ap198:2.4_0": "gacchate anilañjase.",
  "tha-ap198:3.1_0": "Aññaṁ kiñci na passāmi,",
  "tha-ap198:3.2_0": "pūjāyoggaṁ mahesino;",
  "tha-ap198:3.3_0": "Asokaṁ pallavaṁ disvā,",
  "tha-ap198:3.4_0": "ākāse ukkhipiṁ ahaṁ.",
  "tha-ap198:4.1_0": "Buddhassa te kisalayā,",
  "tha-ap198:4.2_0": "gacchato yanti pacchato;",
  "tha-ap198:4.3_0": "Tāhaṁ disvāna saṁvijiṁ,",
  "tha-ap198:4.4_0": "aho buddhassuḷāratā.",
  "tha-ap198:5.1_0": "Catunnavutito kappe,",
  "tha-ap198:5.2_0": "pallavaṁ abhiropayiṁ;",
  "tha-ap198:5.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap198:5.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap198:6.1_0": "Sattatiṁse ito kappe,",
  "tha-ap198:6.2_0": "eko ekissaro ahu;",
  "tha-ap198:6.3_0": "Sattaratanasampanno,",
  "tha-ap198:6.4_0": "cakkavattī mahabbalo.",
  "tha-ap198:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap198:7.2_0": "…pe…",
  "tha-ap198:7.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap198:8.1_0": "Itthaṁ sudaṁ āyasmā kisalayapūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap198:9.1_0": "Kisalayapūjakattherassāpadānaṁ chaṭṭhaṁ."
}