{
  "thag1.65:0.1_0": "Theragāthā",
  "thag1.65:0.2_0": "Ekakanipāta",
  "thag1.65:0.3_0": "Sattamavagga",
  "thag1.65:0.4_0": "1.65 Ukkhepakatavacchattheragāthā",
  "thag1.65:1.1_0": "“Ukkhepakatavacchassa,",
  "thag1.65:1.2_0": "Saṅkalitaṁ bahūhi vassehi;",
  "thag1.65:1.3_0": "Taṁ bhāsati gahaṭṭhānaṁ,",
  "thag1.65:1.4_0": "Sunisinno uḷārapāmojjo”ti.",
  "thag1.65:1.5_0": "… Ukkhepakatavaccho thero …."
}