{
  "tha-ap206:0.1_0": "Therāpadāna",
  "tha-ap206:0.2_0": "Kaṇikārapupphiyavagga",
  "tha-ap206:0.3_0": "4. Taraṇiyattheraapadāna",
  "tha-ap206:1.1_0": "“Atthadassī tu bhagavā,",
  "tha-ap206:1.2_0": "dvipadindo narāsabho;",
  "tha-ap206:1.3_0": "Purakkhato sāvakehi,",
  "tha-ap206:1.4_0": "gaṅgātīramupāgami.",
  "tha-ap206:2.1_0": "Samatitti kākapeyyā,",
  "tha-ap206:2.2_0": "gaṅgā āsi duruttarā;",
  "tha-ap206:2.3_0": "Uttārayiṁ bhikkhusaṅghaṁ,",
  "tha-ap206:2.4_0": "buddhañca dvipaduttamaṁ.",
  "tha-ap206:3.1_0": "Aṭṭhārase kappasate,",
  "tha-ap206:3.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap206:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap206:3.4_0": "taraṇāya idaṁ phalaṁ.",
  "tha-ap206:4.1_0": "Teraseto kappasate,",
  "tha-ap206:4.2_0": "pañca sabbobhavā ahuṁ;",
  "tha-ap206:4.3_0": "Sattaratanasampannā,",
  "tha-ap206:4.4_0": "cakkavattī mahabbalā.",
  "tha-ap206:5.1_0": "Pacchime ca bhave asmiṁ,",
  "tha-ap206:5.2_0": "jātohaṁ brāhmaṇe kule;",
  "tha-ap206:5.3_0": "Saddhiṁ tīhi sahāyehi,",
  "tha-ap206:5.4_0": "pabbajiṁ satthu sāsane.",
  "tha-ap206:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap206:6.2_0": "…pe…",
  "tha-ap206:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap206:7.1_0": "Itthaṁ sudaṁ āyasmā taraṇiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap206:8.1_0": "Taraṇiyattherassāpadānaṁ catutthaṁ."
}