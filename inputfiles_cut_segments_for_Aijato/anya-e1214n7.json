{
  "anya-e1214n7:0_0": "Tanādayo ",
  "anya-e1214n7:1_0": "120.",
  "anya-e1214n7:2_0": "Tanu vitthāre saka sattismiṁ-du paritāpe sanu dānasmiṁ,",
  "anya-e1214n7:3_0": "Vana yācāyaṁ manu bodhasmiṁ-hi gate apa pāpuṇanasmiṁ (hi,)",
  "anya-e1214n7:4_0": "Kara karaṇasmiṁ(bhavati)si bandhe-su abhissavane(tanu ādīni;)",
  "anya-e1214n7:5_0": "Niccaṁ ṇeṇayantā curādayo.",
  "anya-e1214n7:6_0": "121.",
  "anya-e1214n7:7_0": "Cura theyye loka (dhātu) dassane aki lakkhaṇe,",
  "anya-e1214n7:8_0": "Siyā thaka patighāte (puna) takka vitakkaṇe;",
  "anya-e1214n7:9_0": "122.",
  "anya-e1214n7:10_0": "Lakkha dassanaaṅkesu (vattate) makkha makkhaṇe,",
  "anya-e1214n7:11_0": "Bhakkhā’dane mokkha moce sukha-dukkha (ca) takiraye;",
  "anya-e1214n7:12_0": "123.",
  "anya-e1214n7:13_0": "Liṅga cittakirayā’dīsu maga-magga gavesane,",
  "anya-e1214n7:14_0": "(Punā’pi) paca vitthāre klese vañca palambhane.",
  "anya-e1214n7:15_0": "124.",
  "anya-e1214n7:16_0": "Vacca ajjhāyane acca pūjāyaṁ vaca bhāsane,",
  "anya-e1214n7:17_0": "Raca patiyatane suca pesuññe ruca rocane;",
  "anya-e1214n7:18_0": "125.",
  "anya-e1214n7:19_0": "Mucappamocane loca dassane kaca dittiyaṁ,",
  "anya-e1214n7:20_0": "Sajjā’jja ajjane tajja tajjane vajja vajjane;",
  "anya-e1214n7:21_0": "126.",
  "anya-e1214n7:22_0": "Yuja saṁyamane pūja pūjāyaṁ tija tejane,",
  "anya-e1214n7:23_0": "Paja magga saṁvaraṇe gate bhaja vibhājane;",
  "anya-e1214n7:24_0": "127.",
  "anya-e1214n7:25_0": "(Atho) bhāja puthakkāre sabhāja pītidassane,",
  "anya-e1214n7:26_0": "(Atho tu) ghaṭa saṅghāte ghaṭṭa sañcalanā’disu;",
  "anya-e1214n7:27_0": "128.",
  "anya-e1214n7:28_0": "Kuṭa-koṭṭacchedane (dve) kuṭa ākoṭanā’disu,",
  "anya-e1214n7:29_0": "Naṭa nacce caṭa-puṭa bhede vaṇṭa vibhājane;",
  "anya-e1214n7:30_0": "129.",
  "anya-e1214n7:31_0": "Tuvaṭṭa ekasayane ghaṭo visaraṇe (siyā),",
  "anya-e1214n7:32_0": "Guṇṭha oguṇṭhane heṭha bādhāyaṁ veṭha veṭhane",
  "anya-e1214n7:33_0": "Guḍi veṭhe kaḍi-khaḍi bhedane maḍi bhūsane;",
  "anya-e1214n7:34_0": "130.",
  "anya-e1214n7:35_0": "Paṇḍa-bhaṇḍa paribhāse daḍi āṇāya (mīrito),",
  "anya-e1214n7:36_0": "Taḍi saṁtāḷane piṇḍa saṅghāte chaḍḍa chaḍḍane;",
  "anya-e1214n7:37_0": "131.",
  "anya-e1214n7:38_0": "Vaṇṇa saṁvaṇṇane cuṇṇa cuṇṇane āṇa pesane,",
  "anya-e1214n7:39_0": "Gaṇa saṁkalane kaṇṇa savaṇe cinta cintane;",
  "anya-e1214n7:40_0": "132.",
  "anya-e1214n7:41_0": "Santa saṅkocane manta gutta bhāsana jānane,",
  "anya-e1214n7:42_0": "Cita saṁcetanā’disu kitta saṁsaddane (bhave;)",
  "anya-e1214n7:43_0": "133.",
  "anya-e1214n7:44_0": "Yata nīyyātane gantha sandabbhe attha yācane,",
  "anya-e1214n7:45_0": "Katha vākyappabandhe (ca) vida ñāṇe nude cuda;",
  "anya-e1214n7:46_0": "134.",
  "anya-e1214n7:47_0": "Chadā’pavāraṇe chadda vamane chanda icchayaṁ,",
  "anya-e1214n7:48_0": "Vadī’bhivāda thomesu bhadikalyāṇakammani;",
  "anya-e1214n7:49_0": "135.",
  "anya-e1214n7:50_0": "Hiḷāda (tu) sukhe gandha sūcane vidha kampane,",
  "anya-e1214n7:51_0": "Randha pāke (atho) māna pūjāyaṁ nu tthutimhi (tu;)",
  "anya-e1214n7:52_0": "136.",
  "anya-e1214n7:53_0": "Thana devasadde ūna parihāne thena coriye,",
  "anya-e1214n7:54_0": "Dhana sadde ñapa tosa nisāna māraṇā’disu;",
  "anya-e1214n7:55_0": "137.",
  "anya-e1214n7:56_0": "Lapa vākye jhapa dāhe rupa ropaṇaādisu,",
  "anya-e1214n7:57_0": "Pī tappane (siyā) kappa vitakke labhi vañcane;",
  "anya-e1214n7:58_0": "138.",
  "anya-e1214n7:59_0": "(Atho) vahi garahāyaṁ samu sāntvana dassane,",
  "anya-e1214n7:60_0": "Kamu icchāya kantimhi (siyā) thoma silāghane;",
  "anya-e1214n7:61_0": "139.",
  "anya-e1214n7:62_0": "Timu temana saṅkāsu ama rogagatā’disu,",
  "anya-e1214n7:63_0": "Saṁgāma yuddhe (vatteyya) īra vācā pakampane;",
  "anya-e1214n7:64_0": "140.",
  "anya-e1214n7:65_0": "Vara āvaraṇi’cchāsu yācāyaṁ dhara dhāraṇe,",
  "anya-e1214n7:66_0": "Tīra kamma samattimhi pāra sāmatthiyā’disu;",
  "anya-e1214n7:67_0": "141.",
  "anya-e1214n7:68_0": "Tulu’mmāne khala sove sañcaye pālarakkhaṇe,",
  "anya-e1214n7:69_0": "Kala saṅkalanā’dīsu (bhave) mīla nimīlane;",
  "anya-e1214n7:70_0": "142.",
  "anya-e1214n7:71_0": "Sīlū’padhāraṇe mūla rohaṇe lala icchane,",
  "anya-e1214n7:72_0": "Dula ukkhepaṇe pūla mahattana samussaye;",
  "anya-e1214n7:73_0": "143.",
  "anya-e1214n7:74_0": "Ghusa sadde pisa pese bhusā’laṅkaraṇe (siyā,)",
  "anya-e1214n7:75_0": "Rusa pārusiye khuṁsa akkose pusa posane;",
  "anya-e1214n7:76_0": "144.",
  "anya-e1214n7:77_0": "Disa uccāraṇā’dīsu vasa acchādane (siyā,)",
  "anya-e1214n7:78_0": "Rasa’ssāde rave snehe (atho) sisa visesane;",
  "anya-e1214n7:79_0": "145.",
  "anya-e1214n7:80_0": "Si bandhe missa sammisse kuha vimbhāpane siyā,",
  "anya-e1214n7:81_0": "Raha cāge gate (cā’pi) maha pūjāya (mīrito;)",
  "anya-e1214n7:82_0": "146.",
  "anya-e1214n7:83_0": "Pihi’cchāyaṁ siyā vīḷa lajjāyaṁ eḷa phāḷane",
  "anya-e1214n7:84_0": "Hīḷa gārahiye pīḷa bādhāyaṁ taḷa tāḷane",
  "anya-e1214n7:85_0": "Laḷa (dhātū)’pasevā’yaṁ (vattatī’mecurādayo;)",
  "anya-e1214n7:86_0": "Samattā sattagaṇā.",
  "anya-e1214n7:87_0": "147.",
  "anya-e1214n7:88_0": "Bhuvādī ca rudhādī ca-divādi svā’dayo gaṇā,",
  "anya-e1214n7:89_0": "Kiyādī ca tanādī ca-curādītī’dha sattadhā;",
  "anya-e1214n7:90_0": "148.",
  "anya-e1214n7:91_0": "Kirayāvācittamakkhātu-me’kekattho bahū’dito,",
  "anya-e1214n7:92_0": "Payogato’nugantabbā-anekatthā hi dhātavo;",
  "anya-e1214n7:93_0": "149.",
  "anya-e1214n7:94_0": "Hitāya mandabuddhīnaṁ-vyattaṁ vaṇṇakkamā lahuṁ,",
  "anya-e1214n7:95_0": "Racitā dhātumañjusā-sīlavaṁsena dhīmatā;",
  "anya-e1214n7:96_0": "150.",
  "anya-e1214n7:97_0": "Saddhammapaṅkeruharājahaṁso,",
  "anya-e1214n7:98_0": "Āsiṭṭhadhammaṭṭhiti sīlavaṁso;",
  "anya-e1214n7:99_0": "Yakkhaddilenākhya nivāsavāsī,",
  "anya-e1214n7:100_0": "Yatissaro soyamidaṁ akāsi;",
  "anya-e1214n7:101_0": "Kaccāyana dhātumañjūsā samattā.",
  "anya-e1214n7:102_0": "Sācariyānusiṭṭhā parisiṭṭhaparibhāsā",
  "anya-e1214n7:103_0": "1.",
  "anya-e1214n7:104_0": "Ekā nekassa rānantū-’bhayesaṁ antimā sarā,",
  "anya-e1214n7:105_0": "Aṅgānubandhā dhātūnaṁ-vuccante’pi yathākkamaṁ;",
  "anya-e1214n7:106_0": "2.",
  "anya-e1214n7:107_0": "Dhātuno vyāñjanā pubbe-niggahītaṁ sama’ntimā,",
  "anya-e1214n7:108_0": "Ivaṇṇenā’rudhādīna-manubandhena ciṇhitaṁ;",
  "anya-e1214n7:109_0": "3.",
  "anya-e1214n7:110_0": "Sesā’nubandhā sabbesaṁ-hontī’dhu’ccāraṇapphalā,",
  "anya-e1214n7:111_0": "Uccāvacapphalā bhāsa-ntarampatvā bha vanti’pi;",
  "anya-e1214n7:112_0": "4.",
  "anya-e1214n7:113_0": "Nāmadhātuka bhāvo’pi-kirayāya adhikārato,",
  "anya-e1214n7:114_0": "Viruddhantarābhāvā-kvacideva payujjate;",
  "anya-e1214n7:115_0": "5.",
  "anya-e1214n7:116_0": "Dvandayuttivasā kvāpi-ādeso yovibhattiyā,",
  "anya-e1214n7:117_0": "Guṇādibhāva saddo’pi-takirayatthe vidhīyate;"
}