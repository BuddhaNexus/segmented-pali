{
  "tha-ap317:0.1_0": "Therāpadāna",
  "tha-ap317:0.2_0": "Ārakkhadāyakavagga",
  "tha-ap317:0.3_0": "5. Pupphāsanadāyakattheraapadāna",
  "tha-ap317:1.1_0": "“Suvaṇṇavaṇṇaṁ sambuddhaṁ,",
  "tha-ap317:1.2_0": "Pītaraṁsiṁva bhāṇumaṁ;",
  "tha-ap317:1.3_0": "Avidūrena gacchantaṁ,",
  "tha-ap317:1.4_0": "Siddhatthaṁ aparājitaṁ.",
  "tha-ap317:2.1_0": "Tassa paccuggamitvāna,",
  "tha-ap317:2.2_0": "pavesetvāna assamaṁ;",
  "tha-ap317:2.3_0": "Pupphāsanaṁ mayā dinnaṁ,",
  "tha-ap317:2.4_0": "vippasannena cetasā.",
  "tha-ap317:3.1_0": "Añjaliṁ paggahetvāna,",
  "tha-ap317:3.2_0": "vedajāto tadā ahaṁ;",
  "tha-ap317:3.3_0": "Buddhe cittaṁ pasādetvā,",
  "tha-ap317:3.4_0": "taṁ kammaṁ pariṇāmayiṁ.",
  "tha-ap317:4.1_0": "Yaṁ me atthi kataṁ puññaṁ,",
  "tha-ap317:4.2_0": "sayambhumhaparājite;",
  "tha-ap317:4.3_0": "Sabbena tena kusalena,",
  "tha-ap317:4.4_0": "vimalo homi sāsane.",
  "tha-ap317:5.1_0": "Catunnavutito kappe,",
  "tha-ap317:5.2_0": "pupphāsanamadaṁ tadā;",
  "tha-ap317:5.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap317:5.4_0": "pupphāsanassidaṁ phalaṁ.",
  "tha-ap317:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap317:6.2_0": "…pe…",
  "tha-ap317:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap317:7.1_0": "Itthaṁ sudaṁ āyasmā pupphāsanadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap317:8.1_0": "Pupphāsanadāyakattherassāpadānaṁ pañcamaṁ."
}