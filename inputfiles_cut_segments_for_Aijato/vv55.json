{
  "vv55:0.1_0": "Vimānavatthu",
  "vv55:0.2_0": "Purisavimāna",
  "vv55:0.3_0": "Mahārathavagga",
  "vv55:0.4_0": "5. Dvārapālavimānavatthu",
  "vv55:1.1_0": "“Uccamidaṁ maṇithūṇaṁ vimānaṁ,",
  "vv55:1.2_0": "Samantato dvādasa yojanāni;",
  "vv55:1.3_0": "Kūṭāgārā sattasatā uḷārā,",
  "vv55:1.4_0": "Veḷuriyathambhā rucakatthatā subhā.",
  "vv55:2.1_0": "Tatthacchasi pivasi khādasi ca,",
  "vv55:2.2_0": "Dibbā ca vīṇā pavadanti vagguṁ;",
  "vv55:2.3_0": "Dibbā rasā kāmaguṇettha pañca,",
  "vv55:2.4_0": "Nāriyo ca naccanti suvaṇṇachannā.",
  "vv55:3.1_0": "Kena tetādiso vaṇṇo,",
  "vv55:3.2_0": "…pe…",
  "vv55:3.3_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv55:4.1_0": "So devaputto attamano,",
  "vv55:4.2_0": "moggallānena pucchito;",
  "vv55:4.3_0": "Pañhaṁ puṭṭho viyākāsi,",
  "vv55:4.4_0": "yassa kammassidaṁ phalaṁ.",
  "vv55:5.1_0": "“Dibbaṁ mamaṁ vassasahassamāyu,",
  "vv55:5.2_0": "Vācābhigītaṁ manasā pavattitaṁ;",
  "vv55:5.3_0": "Ettāvatā ṭhassati puññakammo,",
  "vv55:5.4_0": "Dibbehi kāmehi samaṅgibhūto.",
  "vv55:6.1_0": "Tena metādiso vaṇṇo,",
  "vv55:6.2_0": "…pe…",
  "vv55:6.3_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv55:7.1_0": "Dvārapālavimānaṁ pañcamaṁ."
}