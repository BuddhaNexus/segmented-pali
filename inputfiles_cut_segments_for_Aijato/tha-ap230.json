{
  "tha-ap230:0.1_0": "Therāpadāna",
  "tha-ap230:0.2_0": "Ālambaṇadāyakavagga",
  "tha-ap230:0.3_0": "8. Miñjavaṭaṁsakiyattheraapadāna",
  "tha-ap230:1.1_0": "“Nibbute lokanāthamhi,",
  "tha-ap230:1.2_0": "sikhimhi vadataṁ vare;",
  "tha-ap230:1.3_0": "Vaṭaṁsakehi ākiṇṇaṁ,",
  "tha-ap230:1.4_0": "bodhipūjaṁ akāsahaṁ.",
  "tha-ap230:2.1_0": "Ekattiṁse ito kappe,",
  "tha-ap230:2.2_0": "yaṁ pūjamakariṁ tadā;",
  "tha-ap230:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap230:2.4_0": "bodhipūjāyidaṁ phalaṁ.",
  "tha-ap230:3.1_0": "Ito chabbīsatikappe,",
  "tha-ap230:3.2_0": "ahuṁ meghabbhanāmako;",
  "tha-ap230:3.3_0": "Sattaratanasampanno,",
  "tha-ap230:3.4_0": "cakkavattī mahabbalo.",
  "tha-ap230:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap230:4.2_0": "…pe…",
  "tha-ap230:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap230:5.1_0": "Itthaṁ sudaṁ āyasmā miñjavaṭaṁsakiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap230:6.1_0": "Miñjavaṭaṁsakiyattherassāpadānaṁ aṭṭhamaṁ."
}