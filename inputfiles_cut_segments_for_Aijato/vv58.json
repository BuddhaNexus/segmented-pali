{
  "vv58:0.1_0": "Vimānavatthu",
  "vv58:0.2_0": "Purisavimāna",
  "vv58:0.3_0": "Mahārathavagga",
  "vv58:0.4_0": "8. Paṭhamasūcivimānavatthu",
  "vv58:1.1_0": "“Uccamidaṁ maṇithūṇaṁ vimānaṁ,",
  "vv58:1.2_0": "Samantato dvādasa yojanāni;",
  "vv58:1.3_0": "Kūṭāgārā sattasatā uḷārā,",
  "vv58:1.4_0": "Veḷuriyathambhā rucakatthatā subhā.",
  "vv58:2.1_0": "Tatthacchasi pivasi khādasi ca,",
  "vv58:2.2_0": "Dibbā ca vīṇā pavadanti vagguṁ;",
  "vv58:2.3_0": "Dibbā rasā kāmaguṇettha pañca,",
  "vv58:2.4_0": "Nāriyo ca naccanti suvaṇṇachannā.",
  "vv58:3.1_0": "Kena tetādiso vaṇṇo,",
  "vv58:3.2_0": "…pe…",
  "vv58:3.3_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv58:4.1_0": "So devaputto attamano,",
  "vv58:4.2_0": "…pe…",
  "vv58:4.3_0": "yassa kammassidaṁ phalaṁ.",
  "vv58:5.1_0": "“Yaṁ dadāti na taṁ hoti,",
  "vv58:5.2_0": "Yañceva dajjā tañceva seyyo;",
  "vv58:5.3_0": "Sūci dinnā sūcimeva seyyo.",
  "vv58:6.1_0": "Tena metādiso vaṇṇo,",
  "vv58:6.2_0": "…pe…",
  "vv58:6.3_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv58:7.1_0": "Paṭhamasūcivimānaṁ aṭṭhamaṁ."
}