{
  "thag1.76:0.1_0": "Theragāthā",
  "thag1.76:0.2_0": "Ekakanipāta",
  "thag1.76:0.3_0": "Aṭṭhamavagga",
  "thag1.76:0.4_0": "1.76. Piyañjahattheragāthā",
  "thag1.76:1.1_0": "“Uppatantesu nipate,",
  "thag1.76:1.2_0": "nipatantesu uppate;",
  "thag1.76:1.3_0": "Vase avasamānesu,",
  "thag1.76:1.4_0": "ramamānesu no rame”ti.",
  "thag1.76:1.5_0": "… Piyañjaho thero …."
}