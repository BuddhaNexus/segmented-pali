{
  "thag2.11:0.1_0": "Theragāthā",
  "thag2.11:0.2_0": "Dukanipāta",
  "thag2.11:0.3_0": "Dutiyavagga",
  "thag2.11:0.4_0": "2.11. Mahācundattheragāthā",
  "thag2.11:1.1_0": "“Sussūsā sutavaddhanī,",
  "thag2.11:1.2_0": "sutaṁ paññāya vaddhanaṁ;",
  "thag2.11:1.3_0": "Paññāya atthaṁ jānāti,",
  "thag2.11:1.4_0": "ñāto attho sukhāvaho.",
  "thag2.11:2.1_0": "Sevetha pantāni senāsanāni,",
  "thag2.11:2.2_0": "Careyya saṁyojanavippamokkhaṁ;",
  "thag2.11:2.3_0": "Sace ratiṁ nādhigaccheyya tattha,",
  "thag2.11:2.4_0": "Saṅghe vase rakkhitatto satimā”ti.",
  "thag2.11:2.5_0": "… Mahācundo thero …."
}