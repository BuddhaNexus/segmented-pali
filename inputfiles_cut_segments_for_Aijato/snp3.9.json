{
  "snp3.9:0.1_0": "Sutta Nipāta 3.9",
  "snp3.9:0.2_0": "Vāseṭṭhasutta",
  "snp3.9:1.1_0": "Evaṁ me sutaṁ—",
  "snp3.9:1.2_0": " ekaṁ samayaṁ bhagavā icchānaṅgale viharati icchānaṅgalavanasaṇḍe.",
  "snp3.9:1.3_0": "Tena kho pana samayena sambahulā abhiññātā abhiññātā brāhmaṇamahāsālā icchānaṅgale paṭivasanti, seyyathidaṁ—",
  "snp3.9:1.4_0": "caṅkī brāhmaṇo, tārukkho brāhmaṇo, pokkharasāti brāhmaṇo, jāṇussoṇi brāhmaṇo, todeyyo brāhmaṇo, aññe ca abhiññātā abhiññātā brāhmaṇamahāsālā.",
  "snp3.9:1.5_0": "Atha kho vāseṭṭhabhāradvājānaṁ māṇavānaṁ jaṅghāvihāraṁ anucaṅkamantānaṁ anuvicarantānaṁ ayamantarākathā udapādi:",
  "snp3.9:1.6_0": "“kathaṁ, bho, brāhmaṇo hotī”ti?",
  "snp3.9:2.1_0": "Bhāradvājo māṇavo evamāha:",
  "snp3.9:2.2_0": "“yato kho, bho, ubhato sujāto hoti mātito ca pitito ca saṁsuddhagahaṇiko yāva sattamā pitāmahayugā akkhitto anupakkuṭṭho jātivādena, ettāvatā kho bho brāhmaṇo hotī”ti.",
  "snp3.9:3.1_0": "Vāseṭṭho māṇavo evamāha:",
  "snp3.9:3.2_0": "“yato kho, bho, sīlavā ca hoti vatasampanno ca, ettāvatā kho, bho, brāhmaṇo hotī”ti.",
  "snp3.9:3.3_0": "Neva kho asakkhi bhāradvājo māṇavo vāseṭṭhaṁ māṇavaṁ saññāpetuṁ, na pana asakkhi vāseṭṭho māṇavo bhāradvājaṁ māṇavaṁ saññāpetuṁ.",
  "snp3.9:4.1_0": "Atha kho vāseṭṭho māṇavo bhāradvājaṁ māṇavaṁ āmantesi:",
  "snp3.9:4.2_0": "“ayaṁ kho, bho bhāradvāja, samaṇo gotamo sakyaputto sakyakulā pabbajito icchānaṅgale viharati icchānaṅgalavanasaṇḍe;",
  "snp3.9:4.3_0": "taṁ kho pana bhavantaṁ gotamaṁ evaṁ kalyāṇo kittisaddo abbhuggato:",
  "snp3.9:4.4_0": "‘itipi …pe… buddho bhagavā’ti.",
  "snp3.9:4.5_0": "Āyāma, bho bhāradvāja, yena samaṇo gotamo tenupasaṅkamissāma;",
  "snp3.9:4.5_1": "upasaṅkamitvā samaṇaṁ gotamaṁ etamatthaṁ pucchissāma.",
  "snp3.9:4.6_0": "Yathā no samaṇo gotamo byākarissati tathā naṁ dhāressāmā”ti.",
  "snp3.9:4.7_0": "“Evaṁ, bho”ti kho bhāradvājo māṇavo vāseṭṭhassa māṇavassa paccassosi.",
  "snp3.9:5.1_0": "Atha kho vāseṭṭhabhāradvājā māṇavā yena bhagavā tenupasaṅkamiṁsu;",
  "snp3.9:5.1_1": "upasaṅkamitvā bhagavatā saddhiṁ sammodiṁsu.",
  "snp3.9:5.2_0": "Sammodanīyaṁ kathaṁ sāraṇīyaṁ vītisāretvā ekamantaṁ nisīdiṁsu.",
  "snp3.9:5.3_0": "Ekamantaṁ nisinno kho vāseṭṭho māṇavo bhagavantaṁ gāthāhi ajjhabhāsi:",
  "snp3.9:6.1_0": "“Anuññātapaṭiññātā,",
  "snp3.9:6.2_0": "tevijjā mayamasmubho;",
  "snp3.9:6.3_0": "Ahaṁ pokkharasātissa,",
  "snp3.9:6.4_0": "tārukkhassāyaṁ māṇavo.",
  "snp3.9:7.1_0": "Tevijjānaṁ yadakkhātaṁ,",
  "snp3.9:7.2_0": "Tatra kevalinosmase;",
  "snp3.9:7.3_0": "Padakasma veyyākaraṇā,",
  "snp3.9:7.4_0": "Jappe ācariyasādisā.",
  "snp3.9:8.1_0": "Tesaṁ no jātivādasmiṁ,",
  "snp3.9:8.2_0": "vivādo atthi gotama;",
  "snp3.9:8.3_0": "Jātiyā brāhmaṇo hoti,",
  "snp3.9:8.4_0": "bhāradvājo iti bhāsati;",
  "snp3.9:8.5_0": "Ahañca kammunā brūmi,",
  "snp3.9:8.6_0": "evaṁ jānāhi cakkhuma.",
  "snp3.9:9.1_0": "Te na sakkoma saññāpetuṁ,",
  "snp3.9:9.2_0": "aññamaññaṁ mayaṁ ubho;",
  "snp3.9:9.3_0": "Bhavantaṁ puṭṭhumāgamhā,",
  "snp3.9:9.4_0": "sambuddhaṁ iti vissutaṁ.",
  "snp3.9:10.1_0": "Candaṁ yathā khayātītaṁ,",
  "snp3.9:10.2_0": "pecca pañjalikā janā;",
  "snp3.9:10.3_0": "Vandamānā namassanti,",
  "snp3.9:10.4_0": "evaṁ lokasmi gotamaṁ.",
  "snp3.9:11.1_0": "Cakkhuṁ loke samuppannaṁ,",
  "snp3.9:11.2_0": "mayaṁ pucchāma gotamaṁ;",
  "snp3.9:11.3_0": "Jātiyā brāhmaṇo hoti,",
  "snp3.9:11.4_0": "udāhu bhavati kammunā;",
  "snp3.9:11.5_0": "Ajānataṁ no pabrūhi,",
  "snp3.9:11.6_0": "yathā jānemu brāhmaṇaṁ”.",
  "snp3.9:12.1_0": "“Tesaṁ vo ahaṁ byakkhissaṁ,",
  "snp3.9:12.2_0": "(vāseṭṭhāti bhagavā)",
  "snp3.9:12.3_0": "Anupubbaṁ yathātathaṁ;",
  "snp3.9:12.4_0": "Jātivibhaṅgaṁ pāṇānaṁ,",
  "snp3.9:12.5_0": "Aññamaññā hi jātiyo.",
  "snp3.9:13.1_0": "Tiṇarukkhepi jānātha,",
  "snp3.9:13.2_0": "na cāpi paṭijānare;",
  "snp3.9:13.3_0": "Liṅgaṁ jātimayaṁ tesaṁ,",
  "snp3.9:13.4_0": "aññamaññā hi jātiyo.",
  "snp3.9:14.1_0": "Tato kīṭe paṭaṅge ca,",
  "snp3.9:14.2_0": "yāva kunthakipillike;",
  "snp3.9:14.3_0": "Liṅgaṁ jātimayaṁ tesaṁ,",
  "snp3.9:14.4_0": "aññamaññā hi jātiyo.",
  "snp3.9:15.1_0": "Catuppadepi jānātha,",
  "snp3.9:15.2_0": "Khuddake ca mahallake;",
  "snp3.9:15.3_0": "Liṅgaṁ jātimayaṁ tesaṁ,",
  "snp3.9:15.4_0": "Aññamaññā hi jātiyo.",
  "snp3.9:16.1_0": "Pādūdarepi jānātha,",
  "snp3.9:16.2_0": "Urage dīghapiṭṭhike;",
  "snp3.9:16.3_0": "Liṅgaṁ jātimayaṁ tesaṁ,",
  "snp3.9:16.4_0": "Aññamaññā hi jātiyo.",
  "snp3.9:17.1_0": "Tato macchepi jānātha,",
  "snp3.9:17.2_0": "Odake vārigocare;",
  "snp3.9:17.3_0": "Liṅgaṁ jātimayaṁ tesaṁ,",
  "snp3.9:17.4_0": "Aññamaññā hi jātiyo.",
  "snp3.9:18.1_0": "Tato pakkhīpi jānātha,",
  "snp3.9:18.2_0": "Pattayāne vihaṅgame;",
  "snp3.9:18.3_0": "Liṅgaṁ jātimayaṁ tesaṁ,",
  "snp3.9:18.4_0": "Aññamaññā hi jātiyo.",
  "snp3.9:19.1_0": "Yathā etāsu jātīsu,",
  "snp3.9:19.2_0": "Liṅgaṁ jātimayaṁ puthu;",
  "snp3.9:19.3_0": "Evaṁ natthi manussesu,",
  "snp3.9:19.4_0": "Liṅgaṁ jātimayaṁ puthu.",
  "snp3.9:20.1_0": "Na kesehi na sīsena,",
  "snp3.9:20.2_0": "Na kaṇṇehi na akkhibhi;",
  "snp3.9:20.3_0": "Na mukhena na nāsāya,",
  "snp3.9:20.4_0": "Na oṭṭhehi bhamūhi vā.",
  "snp3.9:21.1_0": "Na gīvāya na aṁsehi,",
  "snp3.9:21.2_0": "Na udarena na piṭṭhiyā;",
  "snp3.9:21.3_0": "Na soṇiyā na urasā,",
  "snp3.9:21.4_0": "Na sambādhe na methune.",
  "snp3.9:22.1_0": "Na hatthehi na pādehi,",
  "snp3.9:22.2_0": "Nāṅgulīhi nakhehi vā;",
  "snp3.9:22.3_0": "Na jaṅghāhi na ūrūhi,",
  "snp3.9:22.4_0": "Na vaṇṇena sarena vā;",
  "snp3.9:22.5_0": "Liṅgaṁ jātimayaṁ neva,",
  "snp3.9:22.6_0": "Yathā aññāsu jātisu.",
  "snp3.9:23.1_0": "Paccattañca sarīresu,",
  "snp3.9:23.2_0": "Manussesvetaṁ na vijjati;",
  "snp3.9:23.3_0": "Vokārañca manussesu,",
  "snp3.9:23.4_0": "Samaññāya pavuccati.",
  "snp3.9:24.1_0": "Yo hi koci manussesu,",
  "snp3.9:24.2_0": "Gorakkhaṁ upajīvati;",
  "snp3.9:24.3_0": "Evaṁ vāseṭṭha jānāhi,",
  "snp3.9:24.4_0": "Kassako so na brāhmaṇo.",
  "snp3.9:25.1_0": "Yo hi koci manussesu,",
  "snp3.9:25.2_0": "Puthusippena jīvati;",
  "snp3.9:25.3_0": "Evaṁ vāseṭṭha jānāhi,",
  "snp3.9:25.4_0": "Sippiko so na brāhmaṇo.",
  "snp3.9:26.1_0": "Yo hi koci manussesu,",
  "snp3.9:26.2_0": "Vohāraṁ upajīvati;",
  "snp3.9:26.3_0": "Evaṁ vāseṭṭha jānāhi,",
  "snp3.9:26.4_0": "Vāṇijo so na brāhmaṇo.",
  "snp3.9:27.1_0": "Yo hi koci manussesu,",
  "snp3.9:27.2_0": "Parapessena jīvati;",
  "snp3.9:27.3_0": "Evaṁ vāseṭṭha jānāhi,",
  "snp3.9:27.4_0": "Pessiko so na brāhmaṇo.",
  "snp3.9:28.1_0": "Yo hi koci manussesu,",
  "snp3.9:28.2_0": "Adinnaṁ upajīvati;",
  "snp3.9:28.3_0": "Evaṁ vāseṭṭha jānāhi,",
  "snp3.9:28.4_0": "Coro eso na brāhmaṇo.",
  "snp3.9:29.1_0": "Yo hi koci manussesu,",
  "snp3.9:29.2_0": "Issatthaṁ upajīvati;",
  "snp3.9:29.3_0": "Evaṁ vāseṭṭha jānāhi,",
  "snp3.9:29.4_0": "Yodhājīvo na brāhmaṇo.",
  "snp3.9:30.1_0": "Yo hi koci manussesu,",
  "snp3.9:30.2_0": "Porohiccena jīvati;",
  "snp3.9:30.3_0": "Evaṁ vāseṭṭha jānāhi,",
  "snp3.9:30.4_0": "Yājako eso na brāhmaṇo.",
  "snp3.9:31.1_0": "Yo hi koci manussesu,",
  "snp3.9:31.2_0": "Gāmaṁ raṭṭhañca bhuñjati;",
  "snp3.9:31.3_0": "Evaṁ vāseṭṭha jānāhi,",
  "snp3.9:31.4_0": "Rājā eso na brāhmaṇo.",
  "snp3.9:32.1_0": "Na cāhaṁ brāhmaṇaṁ brūmi,",
  "snp3.9:32.2_0": "Yonijaṁ mattisambhavaṁ;",
  "snp3.9:32.3_0": "Bhovādi nāma so hoti,",
  "snp3.9:32.4_0": "Sace hoti sakiñcano;",
  "snp3.9:32.5_0": "Akiñcanaṁ anādānaṁ,",
  "snp3.9:32.6_0": "Tamahaṁ brūmi brāhmaṇaṁ.",
  "snp3.9:33.1_0": "Sabbasaṁyojanaṁ chetvā,",
  "snp3.9:33.2_0": "Yo ve na paritassati;",
  "snp3.9:33.3_0": "Saṅgātigaṁ visaṁyuttaṁ,",
  "snp3.9:33.4_0": "Tamahaṁ brūmi brāhmaṇaṁ.",
  "snp3.9:34.1_0": "Chetvā naddhiṁ varattañca,",
  "snp3.9:34.2_0": "Sandānaṁ sahanukkamaṁ;",
  "snp3.9:34.3_0": "Ukkhittapalighaṁ buddhaṁ,",
  "snp3.9:34.4_0": "Tamahaṁ brūmi brāhmaṇaṁ.",
  "snp3.9:35.1_0": "Akkosaṁ vadhabandhañca,",
  "snp3.9:35.2_0": "Aduṭṭho yo titikkhati;",
  "snp3.9:35.3_0": "Khantībalaṁ balānīkaṁ,",
  "snp3.9:35.4_0": "Tamahaṁ brūmi brāhmaṇaṁ.",
  "snp3.9:36.1_0": "Akkodhanaṁ vatavantaṁ,",
  "snp3.9:36.2_0": "Sīlavantaṁ anussadaṁ;",
  "snp3.9:36.3_0": "Dantaṁ antimasārīraṁ,",
  "snp3.9:36.4_0": "Tamahaṁ brūmi brāhmaṇaṁ.",
  "snp3.9:37.1_0": "Vāri pokkharapatteva,",
  "snp3.9:37.2_0": "Āraggeriva sāsapo;",
  "snp3.9:37.3_0": "Yo na limpati kāmesu,",
  "snp3.9:37.4_0": "Tamahaṁ brūmi brāhmaṇaṁ.",
  "snp3.9:38.1_0": "Yo dukkhassa pajānāti,",
  "snp3.9:38.2_0": "Idheva khayamattano;",
  "snp3.9:38.3_0": "Pannabhāraṁ visaṁyuttaṁ,",
  "snp3.9:38.4_0": "Tamahaṁ brūmi brāhmaṇaṁ.",
  "snp3.9:39.1_0": "Gambhīrapaññaṁ medhāviṁ,",
  "snp3.9:39.2_0": "Maggāmaggassa kovidaṁ;",
  "snp3.9:39.3_0": "Uttamatthamanuppattaṁ,",
  "snp3.9:39.4_0": "Tamahaṁ brūmi brāhmaṇaṁ.",
  "snp3.9:40.1_0": "Asaṁsaṭṭhaṁ gahaṭṭhehi,",
  "snp3.9:40.2_0": "Anāgārehi cūbhayaṁ;",
  "snp3.9:40.3_0": "Anokasārimappicchaṁ,",
  "snp3.9:40.4_0": "Tamahaṁ brūmi brāhmaṇaṁ.",
  "snp3.9:41.1_0": "Nidhāya daṇḍaṁ bhūtesu,",
  "snp3.9:41.2_0": "Tasesu thāvaresu ca;",
  "snp3.9:41.3_0": "Yo na hanti na ghāteti,",
  "snp3.9:41.4_0": "Tamahaṁ brūmi brāhmaṇaṁ.",
  "snp3.9:42.1_0": "Aviruddhaṁ viruddhesu,",
  "snp3.9:42.2_0": "Attadaṇḍesu nibbutaṁ;",
  "snp3.9:42.3_0": "Sādānesu anādānaṁ,",
  "snp3.9:42.4_0": "Tamahaṁ brūmi brāhmaṇaṁ.",
  "snp3.9:43.1_0": "Yassa rāgo ca doso ca,",
  "snp3.9:43.2_0": "Māno makkho ca pātito;",
  "snp3.9:43.3_0": "Sāsaporiva āraggā,",
  "snp3.9:43.4_0": "Tamahaṁ brūmi brāhmaṇaṁ.",
  "snp3.9:44.1_0": "Akakkasaṁ viññāpaniṁ,",
  "snp3.9:44.2_0": "Giraṁ saccamudīraye;",
  "snp3.9:44.3_0": "Yāya nābhisaje kañci,",
  "snp3.9:44.4_0": "Tamahaṁ brūmi brāhmaṇaṁ.",
  "snp3.9:45.1_0": "Yodha dīghaṁ va rassaṁ vā,",
  "snp3.9:45.2_0": "Aṇuṁ thūlaṁ subhāsubhaṁ;",
  "snp3.9:45.3_0": "Loke adinnaṁ nādiyati,",
  "snp3.9:45.4_0": "Tamahaṁ brūmi brāhmaṇaṁ.",
  "snp3.9:46.1_0": "Āsā yassa na vijjanti,",
  "snp3.9:46.2_0": "Asmiṁ loke paramhi ca;",
  "snp3.9:46.3_0": "Nirāsāsaṁ visaṁyuttaṁ,",
  "snp3.9:46.4_0": "Tamahaṁ brūmi brāhmaṇaṁ.",
  "snp3.9:47.1_0": "Yassālayā na vijjanti,",
  "snp3.9:47.2_0": "Aññāya akathaṅkathī;",
  "snp3.9:47.3_0": "Amatogadhamanuppattaṁ,",
  "snp3.9:47.4_0": "Tamahaṁ brūmi brāhmaṇaṁ.",
  "snp3.9:48.1_0": "Yodha puññañca pāpañca,",
  "snp3.9:48.2_0": "Ubho saṅgamupaccagā;",
  "snp3.9:48.3_0": "Asokaṁ virajaṁ suddhaṁ,",
  "snp3.9:48.4_0": "Tamahaṁ brūmi brāhmaṇaṁ.",
  "snp3.9:49.1_0": "Candaṁva vimalaṁ suddhaṁ,",
  "snp3.9:49.2_0": "Vippasannamanāvilaṁ;",
  "snp3.9:49.3_0": "Nandībhavaparikkhīṇaṁ,",
  "snp3.9:49.4_0": "Tamahaṁ brūmi brāhmaṇaṁ.",
  "snp3.9:50.1_0": "Yomaṁ palipathaṁ duggaṁ,",
  "snp3.9:50.2_0": "Saṁsāraṁ mohamaccagā;",
  "snp3.9:50.3_0": "Tiṇṇo pāraṅgato jhāyī,",
  "snp3.9:50.4_0": "Anejo akathaṅkathī;",
  "snp3.9:50.5_0": "Anupādāya nibbuto,",
  "snp3.9:50.6_0": "Tamahaṁ brūmi brāhmaṇaṁ.",
  "snp3.9:51.1_0": "Yodha kāme pahantvāna,",
  "snp3.9:51.2_0": "Anāgāro paribbaje;",
  "snp3.9:51.3_0": "Kāmabhavaparikkhīṇaṁ,",
  "snp3.9:51.4_0": "Tamahaṁ brūmi brāhmaṇaṁ.",
  "snp3.9:52.1_0": "Yodha taṇhaṁ pahantvāna,",
  "snp3.9:52.2_0": "Anāgāro paribbaje;",
  "snp3.9:52.3_0": "Taṇhābhavaparikkhīṇaṁ,",
  "snp3.9:52.4_0": "Tamahaṁ brūmi brāhmaṇaṁ.",
  "snp3.9:53.1_0": "Hitvā mānusakaṁ yogaṁ,",
  "snp3.9:53.2_0": "Dibbaṁ yogaṁ upaccagā;",
  "snp3.9:53.3_0": "Sabbayogavisaṁyuttaṁ,",
  "snp3.9:53.4_0": "Tamahaṁ brūmi brāhmaṇaṁ.",
  "snp3.9:54.1_0": "Hitvā ratiñca aratiṁ,",
  "snp3.9:54.2_0": "Sītibhūtaṁ nirūpadhiṁ;",
  "snp3.9:54.3_0": "Sabbalokābhibhuṁ vīraṁ,",
  "snp3.9:54.4_0": "Tamahaṁ brūmi brāhmaṇaṁ.",
  "snp3.9:55.1_0": "Cutiṁ yo vedi sattānaṁ,",
  "snp3.9:55.2_0": "Upapattiñca sabbaso;",
  "snp3.9:55.3_0": "Asattaṁ sugataṁ buddhaṁ,",
  "snp3.9:55.4_0": "Tamahaṁ brūmi brāhmaṇaṁ.",
  "snp3.9:56.1_0": "Yassa gatiṁ na jānanti,",
  "snp3.9:56.2_0": "Devā gandhabbamānusā;",
  "snp3.9:56.3_0": "Khīṇāsavaṁ arahantaṁ,",
  "snp3.9:56.4_0": "Tamahaṁ brūmi brāhmaṇaṁ.",
  "snp3.9:57.1_0": "Yassa pure ca pacchā ca,",
  "snp3.9:57.2_0": "Majjhe ca natthi kiñcanaṁ;",
  "snp3.9:57.3_0": "Akiñcanaṁ anādānaṁ,",
  "snp3.9:57.4_0": "Tamahaṁ brūmi brāhmaṇaṁ.",
  "snp3.9:58.1_0": "Usabhaṁ pavaraṁ vīraṁ,",
  "snp3.9:58.2_0": "Mahesiṁ vijitāvinaṁ;",
  "snp3.9:58.3_0": "Anejaṁ nhātakaṁ buddhaṁ,",
  "snp3.9:58.4_0": "Tamahaṁ brūmi brāhmaṇaṁ.",
  "snp3.9:59.1_0": "Pubbenivāsaṁ yo vedi,",
  "snp3.9:59.2_0": "Saggāpāyañca passati;",
  "snp3.9:59.3_0": "Atho jātikkhayaṁ patto,",
  "snp3.9:59.4_0": "Tamahaṁ brūmi brāhmaṇaṁ.",
  "snp3.9:60.1_0": "Samaññā hesā lokasmiṁ,",
  "snp3.9:60.2_0": "Nāmagottaṁ pakappitaṁ;",
  "snp3.9:60.3_0": "Sammuccā samudāgataṁ,",
  "snp3.9:60.4_0": "Tattha tattha pakappitaṁ.",
  "snp3.9:61.1_0": "Dīgharattamanusayitaṁ,",
  "snp3.9:61.2_0": "Diṭṭhigatamajānataṁ;",
  "snp3.9:61.3_0": "Ajānantā no pabruvanti,",
  "snp3.9:61.4_0": "Jātiyā hoti brāhmaṇo.",
  "snp3.9:62.1_0": "Na jaccā brāhmaṇo hoti,",
  "snp3.9:62.2_0": "Na jaccā hoti abrāhmaṇo;",
  "snp3.9:62.3_0": "Kammunā brāhmaṇo hoti,",
  "snp3.9:62.4_0": "Kammunā hoti abrāhmaṇo.",
  "snp3.9:63.1_0": "Kassako kammunā hoti,",
  "snp3.9:63.2_0": "Sippiko hoti kammunā;",
  "snp3.9:63.3_0": "Vāṇijo kammunā hoti,",
  "snp3.9:63.4_0": "Pessiko hoti kammunā.",
  "snp3.9:64.1_0": "Coropi kammunā hoti,",
  "snp3.9:64.2_0": "Yodhājīvopi kammunā;",
  "snp3.9:64.3_0": "Yājako kammunā hoti,",
  "snp3.9:64.4_0": "Rājāpi hoti kammunā.",
  "snp3.9:65.1_0": "Evametaṁ yathābhūtaṁ,",
  "snp3.9:65.2_0": "Kammaṁ passanti paṇḍitā;",
  "snp3.9:65.3_0": "Paṭiccasamuppādadassā,",
  "snp3.9:65.4_0": "Kammavipākakovidā.",
  "snp3.9:66.1_0": "Kammunā vattati loko,",
  "snp3.9:66.2_0": "Kammunā vattati pajā;",
  "snp3.9:66.3_0": "Kammanibandhanā sattā,",
  "snp3.9:66.4_0": "Rathassāṇīva yāyato.",
  "snp3.9:67.1_0": "Tapena brahmacariyena,",
  "snp3.9:67.2_0": "Saṁyamena damena ca;",
  "snp3.9:67.3_0": "Etena brāhmaṇo hoti,",
  "snp3.9:67.4_0": "Etaṁ brāhmaṇamuttamaṁ.",
  "snp3.9:68.1_0": "Tīhi vijjāhi sampanno,",
  "snp3.9:68.2_0": "santo khīṇapunabbhavo;",
  "snp3.9:68.3_0": "Evaṁ vāseṭṭha jānāhi,",
  "snp3.9:68.4_0": "brahmā sakko vijānatan”ti.",
  "snp3.9:69.1_0": "Evaṁ vutte, vāseṭṭhabhāradvājā māṇavā bhagavantaṁ etadavocuṁ:",
  "snp3.9:69.2_0": "“abhikkantaṁ, bho gotama …pe…",
  "snp3.9:69.3_0": "upāsake no bhavaṁ gotamo dhāretu ajjatagge pāṇupete saraṇaṁ gate”ti.",
  "snp3.9:69.4_0": "Vāseṭṭhasuttaṁ navamaṁ."
}