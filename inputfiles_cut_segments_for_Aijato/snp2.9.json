{
  "snp2.9:0.1_0": "Sutta Nipāta 2.9",
  "snp2.9:0.2_0": "Kiṁsīlasutta",
  "snp2.9:1.1_0": "“Kiṁsīlo kiṁsamācāro,",
  "snp2.9:1.2_0": "kāni kammāni brūhayaṁ;",
  "snp2.9:1.3_0": "Naro sammā niviṭṭhassa,",
  "snp2.9:1.4_0": "uttamatthañca pāpuṇe”.",
  "snp2.9:2.1_0": "“Vuḍḍhāpacāyī anusūyako siyā,",
  "snp2.9:2.2_0": "Kālaññū cassa garūnaṁ dassanāya;",
  "snp2.9:2.3_0": "Dhammiṁ kathaṁ erayitaṁ khaṇaññū,",
  "snp2.9:2.4_0": "Suṇeyya sakkacca subhāsitāni.",
  "snp2.9:3.1_0": "Kālena gacche garūnaṁ sakāsaṁ,",
  "snp2.9:3.2_0": "Thambhaṁ niraṅkatvā nivātavutti;",
  "snp2.9:3.3_0": "Atthaṁ dhammaṁ saṁyamaṁ brahmacariyaṁ,",
  "snp2.9:3.4_0": "Anussare ceva samācare ca.",
  "snp2.9:4.1_0": "Dhammārāmo dhammarato,",
  "snp2.9:4.2_0": "Dhamme ṭhito dhammavinicchayaññū;",
  "snp2.9:4.3_0": "Nevācare dhammasandosavādaṁ,",
  "snp2.9:4.4_0": "Tacchehi nīyetha subhāsitehi.",
  "snp2.9:5.1_0": "Hassaṁ jappaṁ paridevaṁ padosaṁ,",
  "snp2.9:5.2_0": "Māyākataṁ kuhanaṁ giddhi mānaṁ;",
  "snp2.9:5.3_0": "Sārambhaṁ kakkasaṁ kasāvañca mucchaṁ,",
  "snp2.9:5.4_0": "Hitvā care vītamado ṭhitatto.",
  "snp2.9:6.1_0": "Viññātasārāni subhāsitāni,",
  "snp2.9:6.2_0": "Sutañca viññātasamādhisāraṁ;",
  "snp2.9:6.3_0": "Na tassa paññā ca sutañca vaḍḍhati,",
  "snp2.9:6.4_0": "Yo sāhaso hoti naro pamatto.",
  "snp2.9:7.1_0": "Dhamme ca ye ariyapavedite ratā,",
  "snp2.9:7.2_0": "Anuttarā te vacasā manasā kammunā ca;",
  "snp2.9:7.3_0": "Te santisoraccasamādhisaṇṭhitā,",
  "snp2.9:7.4_0": "Sutassa paññāya ca sāramajjhagū”ti.",
  "snp2.9:7.5_0": "Kiṁsīlasuttaṁ navamaṁ."
}