{
  "ja159:0.1_0": "Jātaka",
  "ja159:0.2_0": "Dukanipāta",
  "ja159:0.3_0": "Daḷhavagga",
  "ja159:0.4_0": "9. Morajātaka",
  "ja159:1.1_0": "“Udetayañ cakkhumā ekarājā,",
  "ja159:1.2_0": "Harissavaṇṇo pathavippabhāso;",
  "ja159:1.3_0": "Taṁ taṁ namassāmi harissavaṇṇaṁ pathavippabhāsaṁ,",
  "ja159:1.4_0": "Tayājja guttā viharemu divasaṁ.",
  "ja159:2.1_0": "Ye brāhmaṇā vedagū sabbadhamme,",
  "ja159:2.2_0": "Te me namo te ca maṁ pālayantu;",
  "ja159:2.3_0": "Namatthu buddhānaṁ namatthu bodhiyā,",
  "ja159:2.4_0": "Namo vimuttānaṁ namo vimuttiyā;",
  "ja159:2.5_0": "Imaṁ so parittaṁ katvā,",
  "ja159:2.6_0": "Moro carati esanā.",
  "ja159:3.1_0": "Apetayaṁ cakkhumā ekarājā,",
  "ja159:3.2_0": "Harissavaṇṇo pathavippabhāso;",
  "ja159:3.3_0": "Taṁ taṁ namassāmi harissavaṇṇaṁ pathavippabhāsaṁ,",
  "ja159:3.4_0": "Tayājja guttā viharemu rattiṁ.",
  "ja159:4.1_0": "Ye brāhmaṇā vedagū sabbadhamme,",
  "ja159:4.2_0": "Te me namo te ca maṁ pālayantu;",
  "ja159:4.3_0": "Namatthu buddhānaṁ namatthu bodhiyā,",
  "ja159:4.4_0": "Namo vimuttānaṁ namo vimuttiyā;",
  "ja159:4.5_0": "Imaṁ so parittaṁ katvā,",
  "ja159:4.6_0": "Moro vāsamakappayī”ti.",
  "ja159:5.1_0": "Morajātakaṁ navamaṁ."
}