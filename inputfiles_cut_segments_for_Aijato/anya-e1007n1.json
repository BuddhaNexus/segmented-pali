{
  "anya-e1007n1:0_0": "Sūrassatīnīti",
  "anya-e1007n1:1_0": "1.",
  "anya-e1007n1:2_0": "Kataññutā ca saccañca,",
  "anya-e1007n1:3_0": "Lokasārā hi te duve;",
  "anya-e1007n1:4_0": "Lokāpi tehi tiṭṭhanti,",
  "anya-e1007n1:5_0": "Raṭṭhaṁ akaṁsu issaraṁ.",
  "anya-e1007n1:6_0": "2.",
  "anya-e1007n1:7_0": "Kākāca dujjanā loke,",
  "anya-e1007n1:8_0": "Malībhūtāva sabbadā;",
  "anya-e1007n1:9_0": "Iṭṭhaṁ guṇaṁ nāsayanti,",
  "anya-e1007n1:10_0": "Te ve lokassa verino.",
  "anya-e1007n1:11_0": "Migānaṁ siṅgālo anto,",
  "anya-e1007n1:12_0": "Pakkhīnaṁ pana vāyaso.",
  "anya-e1007n1:13_0": "Akāraṇaverī honti,",
  "anya-e1007n1:14_0": "Macchānaṁ dhīvarā yathā;",
  "anya-e1007n1:15_0": "Guṇīnaṁ sajjanānañca,",
  "anya-e1007n1:16_0": "Dujjanā niccaverino.",
  "anya-e1007n1:17_0": "3.",
  "anya-e1007n1:18_0": "Siriṁ bhonto suposetha,",
  "anya-e1007n1:19_0": "Siri mūlā hi sampadā;",
  "anya-e1007n1:20_0": "Siriya idha jotanti,",
  "anya-e1007n1:21_0": "Sirī sā sabbasiddhikā.",
  "anya-e1007n1:22_0": "4.",
  "anya-e1007n1:23_0": "Musā tamokarā loke,",
  "anya-e1007n1:24_0": "Saccaṁ mīditikārakaṁ;",
  "anya-e1007n1:25_0": "Musātamena dukkhanti,",
  "anya-e1007n1:26_0": "Saccābhāya sukhantive.",
  "anya-e1007n1:27_0": "Suvijānaṁ siṅgālānaṁ,",
  "anya-e1007n1:28_0": "Sakuṇānañca vassitaṁ;",
  "anya-e1007n1:29_0": "Manussavassitaṁ rāja,",
  "anya-e1007n1:30_0": "Dubbijānataraṁ tato.",
  "anya-e1007n1:31_0": "5.",
  "anya-e1007n1:32_0": "Kappaggisadisā issā,",
  "anya-e1007n1:33_0": "Jhāpeti sabbasampadaṁ;",
  "anya-e1007n1:34_0": "Muditā kappameghova,",
  "anya-e1007n1:35_0": "Ropeti sabbasampadaṁ.",
  "anya-e1007n1:36_0": "6.",
  "anya-e1007n1:37_0": "Yathā asanthirā thambhā,",
  "anya-e1007n1:38_0": "Thusarāsimhi ussitā;",
  "anya-e1007n1:39_0": "Tatheva kapicittānaṁ,",
  "anya-e1007n1:40_0": "Kammantā cañcalaṅgatā.",
  "anya-e1007n1:41_0": "Haliddirāgaṁ kapicittaṁ;",
  "anya-e1007n1:42_0": "Purisaṁ rāgavirāginaṁ,",
  "anya-e1007n1:43_0": "Edisaṁ tāta māsevi;",
  "anya-e1007n1:44_0": "Nimmanussampi ce siyā.",
  "anya-e1007n1:45_0": "7. [Ka]",
  "anya-e1007n1:46_0": "Pupphacumbi cittapattī,",
  "anya-e1007n1:47_0": "Sakavaṇṇena majjito;",
  "anya-e1007n1:48_0": "Vikkamī pupphato pupphaṁ,",
  "anya-e1007n1:49_0": "Nassāgāraṁ na saṅgamo.",
  "anya-e1007n1:50_0": "[Kha]",
  "anya-e1007n1:51_0": "Kippilikā dubbaṇṇāpi,",
  "anya-e1007n1:52_0": "Samaggāca parakkamā;",
  "anya-e1007n1:53_0": "Mā hotha pupphacumbīva,",
  "anya-e1007n1:54_0": "Hotha vo pacikā yathā.",
  "anya-e1007n1:55_0": "1.",
  "anya-e1007n1:56_0": "Ye maranti kīḷantā te,",
  "anya-e1007n1:57_0": "Svānā aññoññamoditā;",
  "anya-e1007n1:58_0": "Disvāna chaṭṭitaṁ bhattaṁ,",
  "anya-e1007n1:59_0": "Sīghaverī vihiṁsare.",
  "anya-e1007n1:60_0": "2.",
  "anya-e1007n1:61_0": "Tathekepi janā dāni,",
  "anya-e1007n1:62_0": "Samaggā ññoññamoditā;",
  "anya-e1007n1:63_0": "Dhanahetu vihiṁsanti,",
  "anya-e1007n1:64_0": "Dhīratthu sīghaverikā.",
  "anya-e1007n1:65_0": "9.",
  "anya-e1007n1:66_0": "Vasante hemante gimhe;",
  "anya-e1007n1:67_0": "Neva tālā visesino;",
  "anya-e1007n1:68_0": "Thiracittā janā santā;",
  "anya-e1007n1:69_0": "Sukhadukkhesu niccalā.",
  "anya-e1007n1:70_0": "10.",
  "anya-e1007n1:71_0": "Yathā pavaṭṭamānamhi,",
  "anya-e1007n1:72_0": "Suṭṭhu tiṭṭhati geṇṭhuke;",
  "anya-e1007n1:73_0": "Appavaṭṭe bhūmyaṁ seti,",
  "anya-e1007n1:74_0": "Tatheva geṇṭhuko jano.",
  "anya-e1007n1:75_0": "11.",
  "anya-e1007n1:76_0": "Agghāpetuṁ nasakkonti,",
  "anya-e1007n1:77_0": "Kālañhi kālikā janā;",
  "anya-e1007n1:78_0": "Vajirādiñca sakkonti,",
  "anya-e1007n1:79_0": "Tena kālo anagghiko.",
  "anya-e1007n1:80_0": "12. [Ka]",
  "anya-e1007n1:81_0": "Asano hi dīghaddhāno,",
  "anya-e1007n1:82_0": "Sārasāro sugandhiko;",
  "anya-e1007n1:83_0": "Niggandho tveva nissāro,",
  "anya-e1007n1:84_0": "Dīghaddhānopi simbalī.",
  "anya-e1007n1:85_0": "[Kha]",
  "anya-e1007n1:86_0": "Tatheveke janā loke,",
  "anya-e1007n1:87_0": "Dīghaddhānā susārakā;",
  "anya-e1007n1:88_0": "Nissārā keci pheggūva,",
  "anya-e1007n1:89_0": "Dīghaddhānāpi goyathā.",
  "anya-e1007n1:90_0": "13.",
  "anya-e1007n1:91_0": "Upakāro cāpakāro,",
  "anya-e1007n1:92_0": "Yasmiṁ gacchati naṭṭhataṁ;",
  "anya-e1007n1:93_0": "Pāsāṇahadayassassa,",
  "anya-e1007n1:94_0": "Jīvatītyā bhidhāmudhā.",
  "anya-e1007n1:95_0": "Pasādo nipphalo yassa,",
  "anya-e1007n1:96_0": "Kopocāpi niratthako;",
  "anya-e1007n1:97_0": "Na taṁ saṅgantu miccheyya,",
  "anya-e1007n1:98_0": "Thīpumāva napuṁsakaṁ.",
  "anya-e1007n1:99_0": "14.",
  "anya-e1007n1:100_0": "Upacāro hi kātabbo,",
  "anya-e1007n1:101_0": "Na yāva sohadaṁ bhave;",
  "anya-e1007n1:102_0": "Upacāro sumittamhi,",
  "anya-e1007n1:103_0": "Māyā ca hoti koṭilaṁ.",
  "anya-e1007n1:104_0": "15.",
  "anya-e1007n1:105_0": "Kamena aggato ucchu,",
  "anya-e1007n1:106_0": "Raso sādutaro yathā;",
  "anya-e1007n1:107_0": "Tatheva sumitto loke,",
  "anya-e1007n1:108_0": "Dummitto pana nediso.",
  "anya-e1007n1:109_0": "16.",
  "anya-e1007n1:110_0": "Sokārāti parittāṇaṁ,",
  "anya-e1007n1:111_0": "Vissāsapītibhājanaṁ;",
  "anya-e1007n1:112_0": "Ratanābhiratanaṁ icche,",
  "anya-e1007n1:113_0": "Sumittaṁ akkharattayaṁ.",
  "anya-e1007n1:114_0": "17.",
  "anya-e1007n1:115_0": "Dampatīnaṁ sumittānaṁ,",
  "anya-e1007n1:116_0": "Mukhaṁ aññoññadappaṇaṁ,",
  "anya-e1007n1:117_0": "Sukhe sukhaṁ dukkhe dukkhaṁ,",
  "anya-e1007n1:118_0": "Paṭicchāyeva dappaṇe.",
  "anya-e1007n1:119_0": "Kathaṁ nu tāsaṁ hadayaṁ,",
  "anya-e1007n1:120_0": "Sukharāvata itthiyo;",
  "anya-e1007n1:121_0": "Yāsāmike dukkhitamhi,",
  "anya-e1007n1:122_0": "Sukhamicchanti attano.",
  "anya-e1007n1:123_0": "18.",
  "anya-e1007n1:124_0": "Nivātañca pure katvā,",
  "anya-e1007n1:125_0": "Mānaṁ katvāna pacchato;",
  "anya-e1007n1:126_0": "Sakatthaṁ dhāraye dhīro,",
  "anya-e1007n1:127_0": "Atthabhañjo hi muḷhatā.",
  "anya-e1007n1:128_0": "19.",
  "anya-e1007n1:129_0": "Purecāraṁ satiṁ katvā,",
  "anya-e1007n1:130_0": "Saddhaṁ kareyya pacchato;",
  "anya-e1007n1:131_0": "Turaṁ na saddahe dhīro,",
  "anya-e1007n1:132_0": "Sīghasaddho hi mandako.",
  "anya-e1007n1:133_0": "20.",
  "anya-e1007n1:134_0": "Vane 0.0099 bahūni kaṭṭhāni;",
  "anya-e1007n1:135_0": "Dullabhaṁ rattacandanaṁ;",
  "anya-e1007n1:136_0": "Tathā janā bahū loke;",
  "anya-e1007n1:137_0": "Pumā jañño sudullabho.",
  "anya-e1007n1:138_0": "21.",
  "anya-e1007n1:139_0": "Tiṇakaṭṭhapalāsehi ;",
  "anya-e1007n1:140_0": "Sukkhehi dayhate vanaṁ;",
  "anya-e1007n1:141_0": "Etādīhi asārehi;",
  "anya-e1007n1:142_0": "Loko janehi dayhate.",
  "anya-e1007n1:143_0": "22.",
  "anya-e1007n1:144_0": "Antovasse timāsamhi,",
  "anya-e1007n1:145_0": "Puññakammena moditā;",
  "anya-e1007n1:146_0": "Sukhaṁ vasiṁsu porāṇā,",
  "anya-e1007n1:147_0": "Buddhasāsanamāmakā.",
  "anya-e1007n1:148_0": "23.",
  "anya-e1007n1:149_0": "Migamadena ekena,",
  "anya-e1007n1:150_0": "Taṁ vanaṁ surabhigandhikaṁ;",
  "anya-e1007n1:151_0": "Tathā janena taṁ raṭṭhaṁ,",
  "anya-e1007n1:152_0": "Guṇinā hi sirīmatā.",
  "anya-e1007n1:153_0": "Sarīraṁ khaṇaviddhaṁsī,",
  "anya-e1007n1:154_0": "Kappantaṭṭhāyino guṇā.",
  "anya-e1007n1:155_0": "24.",
  "anya-e1007n1:156_0": "Kharānaṁ sīhabyagghānaṁ,",
  "anya-e1007n1:157_0": "Saṅgamo no hisabbadā;",
  "anya-e1007n1:158_0": "Tatheva byagghacittānaṁ,",
  "anya-e1007n1:159_0": "Sajātikā khayonatā.",
  "anya-e1007n1:160_0": "25.",
  "anya-e1007n1:161_0": "Saka sādhupi no sādhū,",
  "anya-e1007n1:162_0": "Yo ceñña duṭṭhakārako;",
  "anya-e1007n1:163_0": "Bahūnaṁ sādhū pāyena,",
  "anya-e1007n1:164_0": "Sa ve sādhūti vuccate.",
  "anya-e1007n1:165_0": "26.",
  "anya-e1007n1:166_0": "Bahūdake samuddepi,",
  "anya-e1007n1:167_0": "Jalaṁ nattheva pātave;",
  "anya-e1007n1:168_0": "Khuddake khatakūpamhi,",
  "anya-e1007n1:169_0": "Sāduṁ atthi bahuṁ dakaṁ.",
  "anya-e1007n1:170_0": "27.",
  "anya-e1007n1:171_0": "Mā sīghaṁ vivareyyātha,",
  "anya-e1007n1:172_0": "Nindituñca pasaṁsituṁ;",
  "anya-e1007n1:173_0": "Mukhañhi vo kathādvāraṁ,",
  "anya-e1007n1:174_0": "Nirundheyyātha sabbadā.",
  "anya-e1007n1:175_0": "28.",
  "anya-e1007n1:176_0": "Mā sīghaṁ vivareyyātha,",
  "anya-e1007n1:177_0": "Cakkhuṁ vo dassituṁ piyaṁ;",
  "anya-e1007n1:178_0": "Saṇikañhi piyalābhaṁ,",
  "anya-e1007n1:179_0": "Dhanalābhaṁ turaṁ kare.",
  "anya-e1007n1:180_0": "29.",
  "anya-e1007n1:181_0": "Anārambho hi kammānaṁ,",
  "anya-e1007n1:182_0": "Paṭhamaṁ buddhilakkhaṇaṁ;",
  "anya-e1007n1:183_0": "Niṭṭhaṅgataṁ āraddhassa,",
  "anya-e1007n1:184_0": "Dutiyaṁ buddhilakkhaṇaṁ.",
  "anya-e1007n1:185_0": "Asamekkhitakammantaṁ ,",
  "anya-e1007n1:186_0": "Turitābhinipātinaṁ;",
  "anya-e1007n1:187_0": "Sāni kammāni tappenti,",
  "anya-e1007n1:188_0": "Uṇhaṁ vajjhohaṭaṁ mukhe.",
  "anya-e1007n1:189_0": "30.",
  "anya-e1007n1:190_0": "Aphalāni durantāni,",
  "anya-e1007n1:191_0": "Janatā ninditāni ca;",
  "anya-e1007n1:192_0": "Asakyāni ca kammāni,",
  "anya-e1007n1:193_0": "Nārabhetha vicakkhaṇo.",
  "anya-e1007n1:194_0": "31.",
  "anya-e1007n1:195_0": "Ativirodhabhītānaṁ ,",
  "anya-e1007n1:196_0": "Saṅkitānaṁ pade pade;",
  "anya-e1007n1:197_0": "Parappavādatāsānaṁ,",
  "anya-e1007n1:198_0": "Dūrato yanti sampadā.",
  "anya-e1007n1:199_0": "Saddamattaṁ na bhetabbaṁ,",
  "anya-e1007n1:200_0": "Loko saddassa gocaro;",
  "anya-e1007n1:201_0": "Yo ca saddaparittāso,",
  "anya-e1007n1:202_0": "Vane bhantamigo hi so.",
  "anya-e1007n1:203_0": "32.",
  "anya-e1007n1:204_0": "Dvinnaṁ taṇḍulathūsānaṁ,",
  "anya-e1007n1:205_0": "Viseso suṭṭhu khāyati;",
  "anya-e1007n1:206_0": "Randhitopi siniddho no,",
  "anya-e1007n1:207_0": "Thuso virasaphāruso;",
  "anya-e1007n1:208_0": "Taṇḍulaṁ siniddhaṁ rasaṁ,",
  "anya-e1007n1:209_0": "Evaṁ lokepi ñāyate.",
  "anya-e1007n1:210_0": "33.",
  "anya-e1007n1:211_0": "Eraṇḍaṁ nissitā valli,",
  "anya-e1007n1:212_0": "Ruhate kiṁ yathābalaṁ;",
  "anya-e1007n1:213_0": "Mahāsālaṁ sunissāya,",
  "anya-e1007n1:214_0": "Ruhate brahataṁ gatā.",
  "anya-e1007n1:215_0": "34.",
  "anya-e1007n1:216_0": "Mettā hi sīmasambhedā,",
  "anya-e1007n1:217_0": "Pakkhapāta vighātikā;",
  "anya-e1007n1:218_0": "Pakkhapātena dukkhanti,",
  "anya-e1007n1:219_0": "Nippakkho vasate sukhaṁ.",
  "anya-e1007n1:220_0": "35.",
  "anya-e1007n1:221_0": "Narā paññā ca laṅkārā,",
  "anya-e1007n1:222_0": "Yathāṭhāne niyujjare;",
  "anya-e1007n1:223_0": "No hi cūḷāmaṇi pāde,",
  "anya-e1007n1:224_0": "Pādukā ca siropari.",
  "anya-e1007n1:225_0": "Ukkuṭṭhe sūra micchanti;",
  "anya-e1007n1:226_0": "Mantīsu akutūhalaṁ;",
  "anya-e1007n1:227_0": "Viyañca annapānamhi;",
  "anya-e1007n1:228_0": "Atthe jāte ca paṇḍitaṁ.",
  "anya-e1007n1:229_0": "36.",
  "anya-e1007n1:230_0": "Pamādo hi tamo loke,",
  "anya-e1007n1:231_0": "Kālo coro bhayānako;",
  "anya-e1007n1:232_0": "Kāyagehaṁ bahuchiddaṁ,",
  "anya-e1007n1:233_0": "Kālacorassa coritaṁ.",
  "anya-e1007n1:234_0": "37.",
  "anya-e1007n1:235_0": "Cañcalo kāladāso hi,",
  "anya-e1007n1:236_0": "Dhītimā kālaissaro;",
  "anya-e1007n1:237_0": "Kālissaro raṭṭhissaraṁ,",
  "anya-e1007n1:238_0": "Ativattati sabbaso.",
  "anya-e1007n1:239_0": "38.",
  "anya-e1007n1:240_0": "Viluppanti 8 dhanaṁ eke,",
  "anya-e1007n1:241_0": "Kālameke anekkhakā;",
  "anya-e1007n1:242_0": "Tesu kālavilopāva,",
  "anya-e1007n1:243_0": "Bhayānakā tikakkhaḷā.",
  "anya-e1007n1:244_0": "39.",
  "anya-e1007n1:245_0": "Raññā raṭṭhahitaṁ kattā,",
  "anya-e1007n1:246_0": "Rañño hitaṁ janehi ve;",
  "anya-e1007n1:247_0": "Desso attahitaṁ dassī,",
  "anya-e1007n1:248_0": "Gārayho kinnu kārako.",
  "anya-e1007n1:249_0": "Attadatthaṁ paratthena;",
  "anya-e1007n1:250_0": "Bahunāpi na hāpaye.",
  "anya-e1007n1:251_0": "40.",
  "anya-e1007n1:252_0": "Yassa upakāro dinno,",
  "anya-e1007n1:253_0": "Upakāraṁ dade puna;",
  "anya-e1007n1:254_0": "Tato pakāraṁ niccheyya,",
  "anya-e1007n1:255_0": "Kataññū dullabho idha.",
  "anya-e1007n1:256_0": "Saccaṁ kireva māhaṁsu,",
  "anya-e1007n1:257_0": "Narā ekacciyā idha;",
  "anya-e1007n1:258_0": "Kaṭṭhaṁ niplavitaṁ seyyo,",
  "anya-e1007n1:259_0": "Natveve kacciyo naro.",
  "anya-e1007n1:260_0": "41.",
  "anya-e1007n1:261_0": "Kāruko sakapaññāya,",
  "anya-e1007n1:262_0": "Mahagghaṁ dārukaṁ kare;",
  "anya-e1007n1:263_0": "Tathā janopi attānaṁ,",
  "anya-e1007n1:264_0": "Mahaggho lokamānito.",
  "anya-e1007n1:265_0": "42.",
  "anya-e1007n1:266_0": "Appaggho hi ayo hemaṁ,",
  "anya-e1007n1:267_0": "Mahagghaṁ chindate yathā;",
  "anya-e1007n1:268_0": "Nigguṇo saguṇaṁ loke,",
  "anya-e1007n1:269_0": "Alakkhīca siriṁ tathā.",
  "anya-e1007n1:270_0": "Sūrassatīnīti",
  "anya-e1007n1:271_0": "Dutiyo bhāgo",
  "anya-e1007n1:272_0": "1.",
  "anya-e1007n1:273_0": "Dhanassa dubbidhaṁ kiccaṁ,",
  "anya-e1007n1:274_0": "Pāpeti uṇṇataṁ dhaniṁ;",
  "anya-e1007n1:275_0": "Adhaniṁ oṇataṁ loke,",
  "anya-e1007n1:276_0": "Sañcine tena taṁ dhanaṁ.",
  "anya-e1007n1:277_0": "2.",
  "anya-e1007n1:278_0": "Vaṭṭate satataṁ sīghaṁ,",
  "anya-e1007n1:279_0": "Kālacakkaṁ avāritaṁ;",
  "anya-e1007n1:280_0": "Tena ghaṭī dinaṁ māso,",
  "anya-e1007n1:281_0": "Vasso bhavatya cīrato.",
  "anya-e1007n1:282_0": "3.",
  "anya-e1007n1:283_0": "Sattunā na hi sandheyya,",
  "anya-e1007n1:284_0": "Ekadā so bhayaṁ karo;",
  "anya-e1007n1:285_0": "Sutattamapi pānīyaṁ,",
  "anya-e1007n1:286_0": "Samayate nu pāvakaṁ.",
  "anya-e1007n1:287_0": "4.",
  "anya-e1007n1:288_0": "Vijahaṁ pakatiṁ yo hi,",
  "anya-e1007n1:289_0": "Vikatiṁ puna gacchati;",
  "anya-e1007n1:290_0": "Sabhāvena ākārena,",
  "anya-e1007n1:291_0": "Vippallāsaṁ sa gacchati;",
  "anya-e1007n1:292_0": "Saṁsumāra gatā godhā,",
  "anya-e1007n1:293_0": "Yathā thī pumavesikā.",
  "anya-e1007n1:294_0": "5.",
  "anya-e1007n1:295_0": "Pakkhaṁ laddhāna uḍḍenti,",
  "anya-e1007n1:296_0": "Upacikā hi vammikā;",
  "anya-e1007n1:297_0": "Nikkhantā maraṇaṁ yanti,",
  "anya-e1007n1:298_0": "Uppatā nippataṁ gatā.",
  "anya-e1007n1:299_0": "6.",
  "anya-e1007n1:300_0": "Sabbaṁpiyassa dajjeyya,",
  "anya-e1007n1:301_0": "Nissesaṁ piyamānasaṁ;",
  "anya-e1007n1:302_0": "Saddhācittaṁ tu no viññū,",
  "anya-e1007n1:303_0": "Saddhāyiko pakkhalito.",
  "anya-e1007n1:304_0": "7.",
  "anya-e1007n1:305_0": "Sakkoti laṅghituṁ byāmaṁ,",
  "anya-e1007n1:306_0": "Mahussāhena yo hi so;",
  "anya-e1007n1:307_0": "Tadaḍḍhaṁ anussāhena,",
  "anya-e1007n1:308_0": "Nossāho tesu thomito;",
  "anya-e1007n1:309_0": "Mahussāho dukkho loke,",
  "anya-e1007n1:310_0": "Anussāho sadā sukho.",
  "anya-e1007n1:311_0": "8.",
  "anya-e1007n1:312_0": "Dukkhamaṁ akkhamanto yo,",
  "anya-e1007n1:313_0": "Piṭṭhikārīca dukkaraṁ,",
  "anya-e1007n1:314_0": "Kadā labheyya so loke,",
  "anya-e1007n1:315_0": "Sukhamaṁ sukaraṁ mudhā;",
  "anya-e1007n1:316_0": "Paccakkhañhi so kareyya,",
  "anya-e1007n1:317_0": "Dukkhamañcāpi dukkaraṁ.",
  "anya-e1007n1:318_0": "9.",
  "anya-e1007n1:319_0": "Anaggho manusso loke,",
  "anya-e1007n1:320_0": "Tosanāposanādinā;",
  "anya-e1007n1:321_0": "Tena so mahagghaṁ kammaṁ,",
  "anya-e1007n1:322_0": "Kare lokahitāyutaṁ.",
  "anya-e1007n1:323_0": "10.",
  "anya-e1007n1:324_0": "Vātena nappabhijjanti,",
  "anya-e1007n1:325_0": "Ninnā veḷū kasā naḷā;",
  "anya-e1007n1:326_0": "Yathāvātaṁ nagacchanti,",
  "anya-e1007n1:327_0": "Tathā care jane kadā.",
  "anya-e1007n1:328_0": "11.",
  "anya-e1007n1:329_0": "Purato ca pacchato ce,",
  "anya-e1007n1:330_0": "Nissayo natthi passato;",
  "anya-e1007n1:331_0": "Adhikaṁ vīriyaṁ hoti,",
  "anya-e1007n1:332_0": "Attanātho tadā bhave.",
  "anya-e1007n1:333_0": "12.",
  "anya-e1007n1:334_0": "Khaṇaṁ ākhubilaṁ sīho,",
  "anya-e1007n1:335_0": "Pāsāṇasakalākulaṁ;",
  "anya-e1007n1:336_0": "Pappoti nakhabhaṅgaṁvā,",
  "anya-e1007n1:337_0": "Phalaṁvā mūsiko bhave.",
  "anya-e1007n1:338_0": "13.",
  "anya-e1007n1:339_0": "Maggamuḷhā janā andhā,",
  "anya-e1007n1:340_0": "Amaggā maggasaññino;",
  "anya-e1007n1:341_0": "Tatheva duppaññā muḷhā,",
  "anya-e1007n1:342_0": "Tathatthaṁ nāva bujjhare.",
  "anya-e1007n1:343_0": "14.",
  "anya-e1007n1:344_0": "Piyarūpaṁ vīrarūpaṁ,",
  "anya-e1007n1:345_0": "Duvidhā rūpasampadā,",
  "anya-e1007n1:346_0": "Nāriṁ icche piyarūpiṁ,",
  "anya-e1007n1:347_0": "Purisaṁ vīrarūpakaṁ.",
  "anya-e1007n1:348_0": "15.",
  "anya-e1007n1:349_0": "Pajjalanti hi khajjotā,",
  "anya-e1007n1:350_0": "Pakkhacālanakammunā;",
  "anya-e1007n1:351_0": "Kusitā supitā nete,",
  "anya-e1007n1:352_0": "Tathā janāpi kammikā.",
  "anya-e1007n1:353_0": "16.",
  "anya-e1007n1:354_0": "Kummasaṅkocamopamya ,",
  "anya-e1007n1:355_0": "Niggahamapi saṁkhame;",
  "anya-e1007n1:356_0": "Pattakāle tu nītiñño,",
  "anya-e1007n1:357_0": "Kaṇhasappova uṭṭhahe.",
  "anya-e1007n1:358_0": "17.",
  "anya-e1007n1:359_0": "Bhakkhasesaṁ nakhādanti,",
  "anya-e1007n1:360_0": "Sīhā unnatacetasā;",
  "anya-e1007n1:361_0": "Paraṁpi napaṇāmenti,",
  "anya-e1007n1:362_0": "Vuddhikāmā tathā care.",
  "anya-e1007n1:363_0": "18.",
  "anya-e1007n1:364_0": "Na va vasso samuppanno,",
  "anya-e1007n1:365_0": "Khīṇo purāṇahāyano;",
  "anya-e1007n1:366_0": "Navavasse navā mettā,",
  "anya-e1007n1:367_0": "Bhāvitabbā hitesinā.",
  "anya-e1007n1:368_0": "19.",
  "anya-e1007n1:369_0": "Santāpayanti kamayāpyabhujaṁ na rogā,",
  "anya-e1007n1:370_0": "Dummantinaṁ kamupayanti na nītidosā;",
  "anya-e1007n1:371_0": "Kaṁ srī na mānayati kaṁ na ca hanti maccu,",
  "anya-e1007n1:372_0": "Kaṁ thīkatā na visayā paripīḷayanti.",
  "anya-e1007n1:373_0": "(Vasantatilakāgāthā.)",
  "anya-e1007n1:374_0": "20.",
  "anya-e1007n1:375_0": "(Ka) byāmamattena daṇḍena,",
  "anya-e1007n1:376_0": "Yolumbya udakaṁ mine;",
  "anya-e1007n1:377_0": "Agambhīraṁ gambhīraṁvā,",
  "anya-e1007n1:378_0": "Agādhe maññi gambhīraṁ.",
  "anya-e1007n1:379_0": "(Kha)",
  "anya-e1007n1:380_0": "Tathā mando saññāṇena,",
  "anya-e1007n1:381_0": "Agādhe maññi paṇḍitaṁ;",
  "anya-e1007n1:382_0": "Samāsamaṁ na jānāti,",
  "anya-e1007n1:383_0": "Bahvappaṁ tikkhamandataṁ.",
  "anya-e1007n1:384_0": "21.",
  "anya-e1007n1:385_0": "Duṭṭhakamme saṅgamanti,",
  "anya-e1007n1:386_0": "Chekakamme ca no idha;",
  "anya-e1007n1:387_0": "Maccuṁ vahanti sīsena,",
  "anya-e1007n1:388_0": "Te muḷhā muḷhasaṅgamā.",
  "anya-e1007n1:389_0": "22.",
  "anya-e1007n1:390_0": "Duvidho saṅgamo loke,",
  "anya-e1007n1:391_0": "Ujuko kuṭilo bhave;",
  "anya-e1007n1:392_0": "Ujukova pasaṁseyyo,",
  "anya-e1007n1:393_0": "Nohyañño sājasaṅgamo.",
  "anya-e1007n1:394_0": "Te iminā upāyena samaggā sammodamānā mahā bhittipiṭṭhikāya vasanti. (Mahosadhajātaka aṭṭhakathā)",
  "anya-e1007n1:395_0": "23.",
  "anya-e1007n1:396_0": "Yūthikā pupphate nohi,",
  "anya-e1007n1:397_0": "Siñcitāpi punappunaṁ;",
  "anya-e1007n1:398_0": "Pupphate sampatte kāle,",
  "anya-e1007n1:399_0": "Evaṁ dhāretha vīriyaṁ.",
  "anya-e1007n1:400_0": "24.",
  "anya-e1007n1:401_0": "Dhanuccayo dhanakkhepo,",
  "anya-e1007n1:402_0": "Duvidhā hi dhanākati;",
  "anya-e1007n1:403_0": "Dhanuccaye nayo atthi,",
  "anya-e1007n1:404_0": "Dhanakkhepamhi no idha.",
  "anya-e1007n1:405_0": "25.",
  "anya-e1007n1:406_0": "Amātā pitarasaṁ vaḍḍhaṁ,",
  "anya-e1007n1:407_0": "Jūtakārañca cañcalaṁ;",
  "anya-e1007n1:408_0": "Nālapeyya visesaññū,",
  "anya-e1007n1:409_0": "Yadicche siddhi mattano.",
  "anya-e1007n1:410_0": "Haliddirāgaṁ kapicittaṁ,",
  "anya-e1007n1:411_0": "Purisaṁ rāgavirāginaṁ;",
  "anya-e1007n1:412_0": "Edisaṁ tāta māsevi,",
  "anya-e1007n1:413_0": "Nimmanussaṁpi ce siyā.",
  "anya-e1007n1:414_0": "26.",
  "anya-e1007n1:415_0": "Guṇā guṇaññūsu guṇā bhavanti,",
  "anya-e1007n1:416_0": "Te nigguṇaṁ patvā bhavanti dosā;",
  "anya-e1007n1:417_0": "Āsādyatoyā pabhavanti najjo,",
  "anya-e1007n1:418_0": "Samudramāsajja bhavantyapeyyā.",
  "anya-e1007n1:419_0": "(Upajātigāthā)",
  "anya-e1007n1:420_0": "27.",
  "anya-e1007n1:421_0": "Silārūpaṁ nimminanti,",
  "anya-e1007n1:422_0": "Koṭṭetvāna punappunaṁ;",
  "anya-e1007n1:423_0": "Koṭṭakova tathā bālā,",
  "anya-e1007n1:424_0": "Sādhuṁ ovajja nimmitā."
}