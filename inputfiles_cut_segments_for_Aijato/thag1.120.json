{
  "thag1.120:0.1_0": "Theragāthā",
  "thag1.120:0.2_0": "Ekakanipāta",
  "thag1.120:0.3_0": "Dvādasamavagga",
  "thag1.120:0.4_0": "1.120. Isidattattheragāthā",
  "thag1.120:1.1_0": "“Pañcakkhandhā pariññātā,",
  "thag1.120:1.2_0": "tiṭṭhanti chinnamūlakā;",
  "thag1.120:1.3_0": "Dukkhakkhayo anuppatto,",
  "thag1.120:1.4_0": "patto me āsavakkhayo”ti.",
  "thag1.120:1.5_0": "…",
  "thag1.120:1.6_0": "Isidatto thero ….",
  "thag1.120:1.7_0": "vaggo dvādasamo.",
  "thag1.120:2.0_0": "Tassuddānaṁ",
  "thag1.120:2.1_0": "Jento ca vacchagotto ca,",
  "thag1.120:2.2_0": "vaccho ca vanasavhayo;",
  "thag1.120:2.3_0": "Adhimutto mahānāmo,",
  "thag1.120:2.4_0": "pārāpariyo yasopi ca;",
  "thag1.120:2.5_0": "Kimilo vajjiputto ca,",
  "thag1.120:2.6_0": "isidatto mahāyasoti.",
  "thag1.120:2.7_0": "Ekakanipāto niṭṭhito.",
  "thag1.120:3.0_0": "Tatruddānaṁ",
  "thag1.120:3.1_0": "Vīsuttarasataṁ therā,",
  "thag1.120:3.2_0": "katakiccā anāsavā;",
  "thag1.120:3.3_0": "Ekakeva nipātamhi,",
  "thag1.120:3.4_0": "susaṅgītā mahesibhīti."
}