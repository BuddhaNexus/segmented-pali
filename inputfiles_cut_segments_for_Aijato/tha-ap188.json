{
  "tha-ap188:0.1_0": "Therāpadāna",
  "tha-ap188:0.2_0": "Kuṭajapupphiyavagga",
  "tha-ap188:0.3_0": "6 Bodhiupaṭṭhākattheraapadāna",
  "tha-ap188:1.1_0": "“Nagare rammavatiyā,",
  "tha-ap188:1.2_0": "āsiṁ murajavādako;",
  "tha-ap188:1.3_0": "Niccupaṭṭhānayuttomhi,",
  "tha-ap188:1.4_0": "gatohaṁ bodhimuttamaṁ.",
  "tha-ap188:2.1_0": "Sāyaṁ pātaṁ upaṭṭhitvā,",
  "tha-ap188:2.2_0": "sukkamūlena codito;",
  "tha-ap188:2.3_0": "Aṭṭhārasakappasate,",
  "tha-ap188:2.4_0": "duggatiṁ nupapajjahaṁ.",
  "tha-ap188:3.1_0": "Pannarase kappasate,",
  "tha-ap188:3.2_0": "ito āsiṁ janādhipo;",
  "tha-ap188:3.3_0": "Murajo nāma nāmena,",
  "tha-ap188:3.4_0": "cakkavattī mahabbalo.",
  "tha-ap188:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap188:4.2_0": "…pe…",
  "tha-ap188:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap188:5.1_0": "Itthaṁ sudaṁ āyasmā bodhiupaṭṭhāko thero imā gāthāyo abhāsitthāti.",
  "tha-ap188:6.1_0": "Bodhiupaṭṭhākattherassāpadānaṁ chaṭṭhaṁ."
}