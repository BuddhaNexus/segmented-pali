{
  "ya1.2.2:0.1_0": "1 Mūlayamaka",
  "ya1.2.2:0.2_0": "1.2 Niddesa",
  "ya1.2.2:0.3_0": "1.2.2. Hetuvārādiniddesa",
  "ya1.2.2:1.1_0": "Ye keci kusalā dhammā, sabbe te kusalahetūti … ?",
  "ya1.2.2:2.1_0": "Tayoeva kusalahetū, avasesā kusalā dhammā na kusalahetū …pe…",
  "ya1.2.2:2.1_1": "kusalanidānā …",
  "ya1.2.2:2.1_2": "kusalasambhavā …",
  "ya1.2.2:2.1_3": "kusalappabhavā …",
  "ya1.2.2:2.1_4": "kusalasamuṭṭhānā …",
  "ya1.2.2:2.1_5": "kusalāhārā …",
  "ya1.2.2:2.1_6": "kusalārammaṇā …",
  "ya1.2.2:2.1_7": "kusalapaccayā …",
  "ya1.2.2:2.1_8": "kusalasamudayā ….",
  "ya1.2.2:3.1_0": "Ye keci akusalā dhammā …",
  "ya1.2.2:3.1_1": "ye keci abyākatā dhammā …",
  "ya1.2.2:3.1_2": "ye keci nāmā dhammā, sabbe te nāmahetūti …",
  "ya1.2.2:3.1_3": "nāmanidānā …",
  "ya1.2.2:3.1_4": "nāmasambhavā …",
  "ya1.2.2:3.1_5": "nāmappabhavā …",
  "ya1.2.2:3.1_6": "nāmasamuṭṭhānā …",
  "ya1.2.2:3.1_7": "nāmāhārā …",
  "ya1.2.2:3.1_8": "nāmārammaṇā …",
  "ya1.2.2:3.1_9": "nāmapaccayā …",
  "ya1.2.2:3.1_10": "nāmasamudayā ….",
  "ya1.2.2:4.1_0": "Mūlaṁ hetu nidānañca,",
  "ya1.2.2:4.2_0": "Sambhavo pabhavena ca;",
  "ya1.2.2:4.3_0": "Samuṭṭhānāhārārammaṇā,",
  "ya1.2.2:4.4_0": "Paccayo samudayena cāti.",
  "ya1.2.2:5.1_0": "Niddesavāro niṭṭhito.",
  "ya1.2.2:6.1_0": "Mūlayamakapāḷi niṭṭhitā."
