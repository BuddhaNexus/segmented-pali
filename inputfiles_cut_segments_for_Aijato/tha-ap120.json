{
  "tha-ap120:0.1_0": "Therāpadāna",
  "tha-ap120:0.2_0": "Mahāparivāravagga",
  "tha-ap120:0.3_0": "8. Maggasaññakattheraapadāna",
  "tha-ap120:1.1_0": "“Padumuttarabuddhassa,",
  "tha-ap120:1.2_0": "sāvakā vanacārino;",
  "tha-ap120:1.3_0": "Vippanaṭṭhā brahāraññe,",
  "tha-ap120:1.4_0": "andhāva anusuyyare.",
  "tha-ap120:2.1_0": "Anussaritvā sambuddhaṁ,",
  "tha-ap120:2.2_0": "padumuttaranāyakaṁ;",
  "tha-ap120:2.3_0": "Tassa te munino puttā,",
  "tha-ap120:2.4_0": "vippanaṭṭhā mahāvane.",
  "tha-ap120:3.1_0": "Bhavanā oruhitvāna,",
  "tha-ap120:3.2_0": "agamiṁ bhikkhusantikaṁ;",
  "tha-ap120:3.3_0": "Tesaṁ maggañca ācikkhiṁ,",
  "tha-ap120:3.4_0": "bhojanañca adāsahaṁ.",
  "tha-ap120:4.1_0": "Tena kammena dvipadinda,",
  "tha-ap120:4.2_0": "lokajeṭṭha narāsabha;",
  "tha-ap120:4.3_0": "Jātiyā sattavassohaṁ,",
  "tha-ap120:4.4_0": "arahattamapāpuṇiṁ.",
  "tha-ap120:5.1_0": "Sacakkhū nāma nāmena,",
  "tha-ap120:5.2_0": "dvādasa cakkavattino;",
  "tha-ap120:5.3_0": "Sattaratanasampannā,",
  "tha-ap120:5.4_0": "pañcakappasate ito.",
  "tha-ap120:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap120:6.2_0": "…pe…",
  "tha-ap120:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap120:7.1_0": "Itthaṁ sudaṁ āyasmā maggasaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap120:8.1_0": "Maggasaññakattherassāpadānaṁ aṭṭhamaṁ."
}