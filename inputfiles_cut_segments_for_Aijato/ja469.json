{
  "ja469:0.1_0": "Jātaka",
  "ja469:0.2_0": "Dvādasakanipāta",
  "ja469:0.3_0": "Cūḷakuṇālavagga",
  "ja469:0.4_0": "6. Mahākaṇhajātaka",
  "ja469:1.1_0": "“Kaṇho kaṇho ca ghoro ca,",
  "ja469:1.2_0": "sukkadāṭho pabhāsavā;",
  "ja469:1.3_0": "Baddho pañcahi rajjūhi,",
  "ja469:1.4_0": "kiṁ ravi sunakho tava”.",
  "ja469:2.1_0": "“Nāyaṁ migānamatthāya,",
  "ja469:2.2_0": "usīnaka bhavissati;",
  "ja469:2.3_0": "Manussānaṁ anayo hutvā,",
  "ja469:2.4_0": "tadā kaṇho pamokkhati”.",
  "ja469:3.1_0": "“Pattahatthā samaṇakā,",
  "ja469:3.2_0": "muṇḍā saṅghāṭipārutā;",
  "ja469:3.3_0": "Naṅgalehi kasissanti,",
  "ja469:3.4_0": "tadā kaṇho pamokkhati.",
  "ja469:4.1_0": "Tapassiniyo pabbajitā,",
  "ja469:4.2_0": "muṇḍā saṅghāṭipārutā;",
  "ja469:4.3_0": "Yadā loke gamissanti,",
  "ja469:4.4_0": "tadā kaṇho pamokkhati.",
  "ja469:5.1_0": "Dīghottaroṭṭhā jaṭilā,",
  "ja469:5.2_0": "paṅkadantā rajassirā;",
  "ja469:5.3_0": "Iṇaṁ codāya gacchanti,",
  "ja469:5.4_0": "tadā kaṇho pamokkhati.",
  "ja469:6.1_0": "Adhicca vede sāvittiṁ,",
  "ja469:6.2_0": "yaññatantañca brāhmaṇā;",
  "ja469:6.3_0": "Bhatikāya yajissanti,",
  "ja469:6.4_0": "tadā kaṇho pamokkhati.",
  "ja469:7.1_0": "Mātaraṁ pitarañcāpi,",
  "ja469:7.2_0": "jiṇṇakaṁ gatayobbanaṁ;",
  "ja469:7.3_0": "Pahū santo na bharanti,",
  "ja469:7.4_0": "tadā kaṇho pamokkhati.",
  "ja469:8.1_0": "Mātaraṁ pitarañcāpi,",
  "ja469:8.2_0": "jiṇṇakaṁ gatayobbanaṁ;",
  "ja469:8.3_0": "Bālā tumheti vakkhanti,",
  "ja469:8.4_0": "tadā kaṇho pamokkhati.",
  "ja469:9.1_0": "Ācariyabhariyaṁ sakhiṁ,",
  "ja469:9.2_0": "mātulāniṁ pitucchakiṁ;",
  "ja469:9.3_0": "Yadā loke gamissanti,",
  "ja469:9.4_0": "tadā kaṇho pamokkhati.",
  "ja469:10.1_0": "Asicammaṁ gahetvāna,",
  "ja469:10.2_0": "khaggaṁ paggayha brāhmaṇā;",
  "ja469:10.3_0": "Panthaghātaṁ karissanti,",
  "ja469:10.4_0": "tadā kaṇho pamokkhati.",
  "ja469:11.1_0": "Sukkacchavī vedhaverā,",
  "ja469:11.2_0": "thūlabāhū apātubhā;",
  "ja469:11.3_0": "Mittabhedaṁ karissanti,",
  "ja469:11.4_0": "tadā kaṇho pamokkhati.",
  "ja469:12.1_0": "Māyāvino nekatikā,",
  "ja469:12.2_0": "asappurisacintakā;",
  "ja469:12.3_0": "Yadā loke bhavissanti,",
  "ja469:12.4_0": "tadā kaṇho pamokkhatī”ti.",
  "ja469:13.1_0": "Mahākaṇhajātakaṁ chaṭṭhaṁ."
}