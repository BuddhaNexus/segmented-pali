{
  "tha-ap478:0.1_0": "Therāpadāna",
  "tha-ap478:0.2_0": "Naḷamālivagga",
  "tha-ap478:0.3_0": "6. Kusaṭṭhakadāyakattheraapadāna",
  "tha-ap478:1.1_0": "“Kassapassa bhagavato,",
  "tha-ap478:1.2_0": "brāhmaṇassa vusīmato;",
  "tha-ap478:1.3_0": "Pasannacitto sumano,",
  "tha-ap478:1.4_0": "kusaṭṭhakamadāsahaṁ.",
  "tha-ap478:2.1_0": "Imasmiṁyeva kappasmiṁ,",
  "tha-ap478:2.2_0": "kusaṭṭhakamadāsahaṁ;",
  "tha-ap478:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap478:2.4_0": "kusaṭṭhakassidaṁ phalaṁ.",
  "tha-ap478:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap478:3.2_0": "…pe…",
  "tha-ap478:3.3_0": "viharāmi anāsavo.",
  "tha-ap478:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap478:4.2_0": "…pe…",
  "tha-ap478:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap478:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap478:5.2_0": "…pe…",
  "tha-ap478:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap478:6.1_0": "Itthaṁ sudaṁ āyasmā kusaṭṭhakadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap478:7.1_0": "Kusaṭṭhakadāyakattherassāpadānaṁ chaṭṭhaṁ."
}