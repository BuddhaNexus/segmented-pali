{
  "mil3.7.12:0.1_0": "Milindapañha",
  "mil3.7.12:0.2_0": "Arūpadhammavavatthānavagga",
  "mil3.7.12:0.3_0": "12. Samuddapañha",
  "mil3.7.12:1.1_0": "Rājā āha—",
  "mil3.7.12:1.2_0": "“bhante nāgasena, ‘samuddo samuddo’ti vuccati, kena kāraṇena udakaṁ ‘samuddo’ti vuccatī”ti?",
  "mil3.7.12:1.3_0": "Thero āha—",
  "mil3.7.12:1.4_0": "“yattakaṁ, mahārāja, udakaṁ, tattakaṁ loṇaṁ.",
  "mil3.7.12:1.5_0": "Yattakaṁ loṇaṁ, tattakaṁ udakaṁ.",
  "mil3.7.12:1.6_0": "Tasmā ‘samuddo’ti vuccatī”ti.",
  "mil3.7.12:2.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.7.12:3.1_0": "Samuddapañho dvādasamo."
}