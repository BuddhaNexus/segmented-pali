{
  "tha-ap46:0.1_0": "Therāpadāna",
  "tha-ap46:0.2_0": "Upālivagga",
  "tha-ap46:0.3_0": "4. Sanniṭṭhāpakattheraapadāna",
  "tha-ap46:1.1_0": "“Araññe kuṭikaṁ katvā,",
  "tha-ap46:1.2_0": "vasāmi pabbatantare;",
  "tha-ap46:1.3_0": "Lābhālābhena santuṭṭho,",
  "tha-ap46:1.4_0": "yasena ayasena ca.",
  "tha-ap46:2.1_0": "Padumuttaro lokavidū,",
  "tha-ap46:2.2_0": "āhutīnaṁ paṭiggaho;",
  "tha-ap46:2.3_0": "Vasīsatasahassehi,",
  "tha-ap46:2.4_0": "āgacchi mama santikaṁ.",
  "tha-ap46:3.1_0": "Upāgataṁ mahānāgaṁ,",
  "tha-ap46:3.2_0": "jalajuttamanāmakaṁ;",
  "tha-ap46:3.3_0": "Tiṇasantharaṁ paññāpetvā,",
  "tha-ap46:3.4_0": "adāsiṁ satthuno ahaṁ.",
  "tha-ap46:4.1_0": "Pasannacitto sumano,",
  "tha-ap46:4.2_0": "āmaṇḍaṁ pānīyañcahaṁ;",
  "tha-ap46:4.3_0": "Adāsiṁ ujubhūtassa,",
  "tha-ap46:4.4_0": "vippasannena cetasā.",
  "tha-ap46:5.1_0": "Satasahassito kappe,",
  "tha-ap46:5.2_0": "yaṁ dānamadadiṁ tadā;",
  "tha-ap46:5.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap46:5.4_0": "āmaṇḍassa idaṁ phalaṁ.",
  "tha-ap46:6.1_0": "Ekatālīsakappamhi,",
  "tha-ap46:6.2_0": "eko āsiṁ arindamo;",
  "tha-ap46:6.3_0": "Sattaratanasampanno,",
  "tha-ap46:6.4_0": "cakkavattī mahabbalo.",
  "tha-ap46:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap46:7.2_0": "…pe…",
  "tha-ap46:7.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap46:8.1_0": "Itthaṁ sudaṁ āyasmā sanniṭṭhāpako thero imā gāthāyo abhāsitthāti.",
  "tha-ap46:9.1_0": "Sanniṭṭhāpakattherassāpadānaṁ catutthaṁ."
}