{
  "tha-ap508:0.1_0": "Therāpadāna",
  "tha-ap508:0.2_0": "Kaṇikāravagga",
  "tha-ap508:0.3_0": "6. Mātuluṅgaphaladāyakattheraapadāna",
  "tha-ap508:1.1_0": "“Kaṇikāraṁva jalitaṁ,",
  "tha-ap508:1.2_0": "puṇṇamāyeva candimaṁ;",
  "tha-ap508:1.3_0": "Jalantaṁ dīparukkhaṁva,",
  "tha-ap508:1.4_0": "addasaṁ lokanāyakaṁ.",
  "tha-ap508:2.1_0": "Mātuluṅgaphalaṁ gayha,",
  "tha-ap508:2.2_0": "adāsiṁ satthuno ahaṁ;",
  "tha-ap508:2.3_0": "Dakkhiṇeyyassa vīrassa,",
  "tha-ap508:2.4_0": "pasanno sehi pāṇibhi.",
  "tha-ap508:3.1_0": "Ekatiṁse ito kappe,",
  "tha-ap508:3.2_0": "yaṁ phalaṁ adadiṁ tadā;",
  "tha-ap508:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap508:3.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap508:4.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap508:4.2_0": "…pe…",
  "tha-ap508:4.3_0": "viharāmi anāsavo.",
  "tha-ap508:5.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap508:5.2_0": "…pe…",
  "tha-ap508:5.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap508:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap508:6.2_0": "…pe…",
  "tha-ap508:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap508:7.1_0": "Itthaṁ sudaṁ āyasmā mātuluṅgaphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap508:8.1_0": "Mātuluṅgaphaladāyakattherassāpadānaṁ chaṭṭhaṁ."
}