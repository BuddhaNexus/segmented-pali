{
  "atk-s0402a6:0_0": "(7) 2. Sukhavaggavaṇṇanā",
  "atk-s0402a6:1_0": "65.",
  "atk-s0402a6:1_1": "Dutiyassa paṭhame gihisukhanti gihīnaṁ sabbakāmanipphattimūlakaṁ sukhaṁ.",
  "atk-s0402a6:1_2": "Pabbajitasukhanti pabbajitānaṁ pabbajjāmūlakaṁ sukhaṁ.",
  "atk-s0402a6:2_0": "66.",
  "atk-s0402a6:2_1": "Dutiye kāmasukhanti kāme ārabbha uppajjanakasukhaṁ.",
  "atk-s0402a6:2_2": "Nekkhammasukhanti nekkhammaṁ vuccati pabbajjā, taṁ ārabbha uppajjanakasukhaṁ.",
  "atk-s0402a6:3_0": "67.",
  "atk-s0402a6:3_1": "Tatiye upadhisukhanti tebhūmakasukhaṁ.",
  "atk-s0402a6:3_2": "Nirupadhisukhanti lokuttarasukhaṁ.",
  "atk-s0402a6:4_0": "68.",
  "atk-s0402a6:4_1": "Catutthe sāsavasukhanti āsavānaṁ paccayabhūtaṁ vaṭṭasukhaṁ.",
  "atk-s0402a6:4_2": "Anāsavasukhanti tesaṁ apaccayabhūtaṁ vivaṭṭasukhaṁ.",
  "atk-s0402a6:5_0": "69.",
  "atk-s0402a6:5_1": "Pañcame sāmisanti saṁkilesaṁ vaṭṭagāmisukhaṁ.",
  "atk-s0402a6:5_2": "Nirāmisanti nikkilesaṁ vivaṭṭagāmisukhaṁ.",
  "atk-s0402a6:6_0": "70.",
  "atk-s0402a6:6_1": "Chaṭṭhe ariyasukhanti aputhujjanasukhaṁ.",
  "atk-s0402a6:6_2": "Anariyasukhanti puthujjanasukhaṁ.",
  "atk-s0402a6:7_0": "71.",
  "atk-s0402a6:7_1": "Sattame kāyikanti kāyaviññāṇasahajātaṁ.",
  "atk-s0402a6:7_2": "Cetasikanti manodvārikasukhaṁ.",
  "atk-s0402a6:7_3": "Taṁ lokiyalokuttaramissakaṁ kathitaṁ.",
  "atk-s0402a6:8_0": "72.",
  "atk-s0402a6:8_1": "Aṭṭhame sappītikanti paṭhamadutiyajjhānasukhaṁ.",
  "atk-s0402a6:8_2": "Nippītikanti tatiyacatutthajjhānasukhaṁ.",
  "atk-s0402a6:8_3": "Tattha lokiyasappītikato lokiyanippītikaṁ, lokuttarasappītikato ca lokuttaranippītikaṁ agganti evaṁ bhummantaraṁ abhinditvā aggabhāvo veditabbo.",
  "atk-s0402a6:9_0": "73.",
  "atk-s0402a6:9_1": "Navame sātasukhanti tīsu jhānesu sukhaṁ.",
  "atk-s0402a6:9_2": "Upekkhāsukhanti catutthajjhānasukhaṁ.",
  "atk-s0402a6:10_0": "74.",
  "atk-s0402a6:10_1": "Dasame samādhisukhanti appanaṁ vā upacāraṁ vā pattasukhaṁ.",
  "atk-s0402a6:10_2": "Asamādhisukhanti tadubhayaṁ appattasukhaṁ.",
  "atk-s0402a6:11_0": "75.",
  "atk-s0402a6:11_1": "Ekādasame sappītikārammaṇanti sappītikaṁ jhānadvayaṁ paccavekkhantassa uppannasukhaṁ.",
  "atk-s0402a6:11_2": "Nippītikārammaṇepi eseva nayo.",
  "atk-s0402a6:11_3": "Dvādasamepi imināva upāyena attho veditabbo.",
  "atk-s0402a6:12_0": "77.",
  "atk-s0402a6:12_1": "Terasame rūpārammaṇanti rūpāvacaracatutthajjhānārammaṇaṁ, yaṁkiñci rūpaṁ ārabbha uppajjanakaṁ vā.",
  "atk-s0402a6:12_2": "Arūpārammaṇanti arūpāvacarajjhānārammaṇaṁ, yaṁkiñci arūpaṁ ārabbha uppajjanakaṁ vāti.",
  "atk-s0402a6:13_0": "Sukhavaggo dutiyo."
}