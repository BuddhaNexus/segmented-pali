{
  "thag1.56:0.1_0": "Theragāthā",
  "thag1.56:0.2_0": "Ekakanipāta",
  "thag1.56:0.3_0": "Chaṭṭhavagga",
  "thag1.56:0.4_0": "1.56. Kuṭivihārittheragāthā",
  "thag1.56:1.1_0": "“Ko kuṭikāyaṁ bhikkhu kuṭikāyaṁ,",
  "thag1.56:1.2_0": "Vītarāgo susamāhitacitto;",
  "thag1.56:1.3_0": "Evaṁ jānāhi āvuso,",
  "thag1.56:1.4_0": "Amoghā te kuṭikā katā”ti.",
  "thag1.56:1.5_0": "… Kuṭivihāritthero …."
}