{
  "tha-ap117:0.1_0": "Therāpadāna",
  "tha-ap117:0.2_0": "Mahāparivāravagga",
  "tha-ap117:0.3_0": "5. Suvaṇṇapupphiyattheraapadāna",
  "tha-ap117:1.1_0": "“Vipassī nāma bhagavā,",
  "tha-ap117:1.2_0": "lokajeṭṭho narāsabho;",
  "tha-ap117:1.3_0": "Nisinno janakāyassa,",
  "tha-ap117:1.4_0": "desesi amataṁ padaṁ.",
  "tha-ap117:2.1_0": "Tassāhaṁ dhammaṁ sutvāna,",
  "tha-ap117:2.2_0": "dvipadindassa tādino;",
  "tha-ap117:2.3_0": "Soṇṇapupphāni cattāri,",
  "tha-ap117:2.4_0": "buddhassa abhiropayiṁ.",
  "tha-ap117:3.1_0": "Suvaṇṇacchadanaṁ āsi,",
  "tha-ap117:3.2_0": "yāvatā parisā tadā;",
  "tha-ap117:3.3_0": "Buddhābhā ca suvaṇṇābhā,",
  "tha-ap117:3.4_0": "āloko vipulo ahu.",
  "tha-ap117:4.1_0": "Udaggacitto sumano,",
  "tha-ap117:4.2_0": "vedajāto katañjalī;",
  "tha-ap117:4.3_0": "Vittisañjanano tesaṁ,",
  "tha-ap117:4.4_0": "diṭṭhadhammasukhāvaho.",
  "tha-ap117:5.1_0": "Āyācitvāna sambuddhaṁ,",
  "tha-ap117:5.2_0": "vanditvāna ca subbataṁ;",
  "tha-ap117:5.3_0": "Pāmojjaṁ janayitvāna,",
  "tha-ap117:5.4_0": "sakaṁ bhavanupāgamiṁ.",
  "tha-ap117:6.1_0": "Bhavane upaviṭṭhohaṁ,",
  "tha-ap117:6.2_0": "buddhaseṭṭhaṁ anussariṁ;",
  "tha-ap117:6.3_0": "Tena cittappasādena,",
  "tha-ap117:6.4_0": "tusitaṁ upapajjahaṁ.",
  "tha-ap117:7.1_0": "Ekanavutito kappe,",
  "tha-ap117:7.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap117:7.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap117:7.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap117:8.1_0": "Soḷasāsiṁsu rājāno,",
  "tha-ap117:8.2_0": "nemisammatanāmakā;",
  "tha-ap117:8.3_0": "Tetālīse ito kappe,",
  "tha-ap117:8.4_0": "cakkavattī mahabbalā.",
  "tha-ap117:9.1_0": "Paṭisambhidā catasso,",
  "tha-ap117:9.2_0": "…pe…",
  "tha-ap117:9.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap117:10.1_0": "Itthaṁ sudaṁ āyasmā suvaṇṇapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap117:11.1_0": "Suvaṇṇapupphiyattherassāpadānaṁ pañcamaṁ."
}