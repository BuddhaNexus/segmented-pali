{
  "ja307:0.1_0": "Jātaka",
  "ja307:0.2_0": "Catukkanipāta",
  "ja307:0.3_0": "Kāliṅgavagga",
  "ja307:0.4_0": "7. Palāsajātaka",
  "ja307:1.1_0": "“Acetanaṁ brāhmaṇa assuṇantaṁ,",
  "ja307:1.2_0": "Jāno ajānantamimaṁ palāsaṁ;",
  "ja307:1.3_0": "Āraddhaviriyo dhuvaṁ appamatto,",
  "ja307:1.4_0": "Sukhaseyyaṁ pucchasi kissa hetu”.",
  "ja307:2.1_0": "“Dūre suto ceva brahā ca rukkho,",
  "ja307:2.2_0": "Dese ṭhito bhūtanivāsarūpo;",
  "ja307:2.3_0": "Tasmā namassāmi imaṁ palāsaṁ,",
  "ja307:2.4_0": "Ye cettha bhūtā te dhanassa hetu”.",
  "ja307:3.1_0": "“So te karissāmi yathānubhāvaṁ,",
  "ja307:3.2_0": "Kataññutaṁ brāhmaṇa pekkhamāno;",
  "ja307:3.3_0": "Kathañhi āgamma sataṁ sakāse,",
  "ja307:3.4_0": "Moghāni te assu pariphanditāni.",
  "ja307:4.1_0": "Yo tindukarukkhassa paro pilakkho,",
  "ja307:4.2_0": "Parivārito pubbayañño uḷāro;",
  "ja307:4.3_0": "Tassesa mūlasmiṁ nidhi nikhāto,",
  "ja307:4.4_0": "Adāyādo gaccha taṁ uddharāhī”ti.",
  "ja307:5.1_0": "Palāsajātakaṁ sattamaṁ."
}