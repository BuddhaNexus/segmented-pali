{
  "ja262:0.1_0": "Jātaka",
  "ja262:0.2_0": "Tikanipāta",
  "ja262:0.3_0": "Padumavagga",
  "ja262:0.4_0": "2. Mudupāṇijātaka",
  "ja262:1.1_0": "“Pāṇi ce muduko cassa,",
  "ja262:1.2_0": "nāgo cassa sukārito;",
  "ja262:1.3_0": "Andhakāro ca vasseyya,",
  "ja262:1.4_0": "atha nūna tadā siyā”.",
  "ja262:2.1_0": "“Analā mudusambhāsā,",
  "ja262:2.2_0": "duppūrā tā nadīsamā;",
  "ja262:2.3_0": "Sīdanti naṁ viditvāna,",
  "ja262:2.4_0": "ārakā parivajjaye.",
  "ja262:3.1_0": "Yaṁ etā upasevanti,",
  "ja262:3.2_0": "chandasā vā dhanena vā;",
  "ja262:3.3_0": "Jātavedova saṁ ṭhānaṁ,",
  "ja262:3.4_0": "khippaṁ anudahanti nan”ti.",
  "ja262:4.1_0": "Mudupāṇijātakaṁ dutiyaṁ."
}