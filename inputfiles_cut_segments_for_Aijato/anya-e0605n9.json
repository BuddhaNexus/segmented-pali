{
  "anya-e0605n9:0_0": "Nekkhammajjhāsayadīpanīyamakagāthā",
  "anya-e0605n9:1_0": " Namo tassa yato mahimato yassa tamo na",
  "anya-e0605n9:2_0": "49.",
  "anya-e0605n9:3_0": " Disvā nimittāni madacchidāni,",
  "anya-e0605n9:4_0": "Thīnaṁ virūpāni ratacchidāni;",
  "anya-e0605n9:5_0": "Pāpāni kammāni sukhacchidāni,",
  "anya-e0605n9:6_0": "Laddhāni ñāṇāni bhavacchidāni.",
  "anya-e0605n9:7_0": "50.",
  "anya-e0605n9:8_0": " Padittagehā viya bheravaṁ ravaṁ,",
  "anya-e0605n9:9_0": "Ravaṁ sammuṭṭhāya gato mahesi;",
  "anya-e0605n9:10_0": "Mahesimolokiyaputtamattano,",
  "anya-e0605n9:11_0": "Tanosi no pemamahoghamattano.",
  "anya-e0605n9:12_0": "51.",
  "anya-e0605n9:13_0": " Ummāraummāragatuddharitvā,",
  "anya-e0605n9:14_0": "Padaṁ padaṁ yātanarāsabhassa;",
  "anya-e0605n9:15_0": "Alaṁ alaṁkāratarena gantuṁ,",
  "anya-e0605n9:16_0": "Matī matīvetimanaṅgabhaṅge.",
  "anya-e0605n9:17_0": "52.",
  "anya-e0605n9:18_0": " Ummāraummāragato mahesi,",
  "anya-e0605n9:19_0": "Anaṅgabhaṅgaṁ samacintayittha;",
  "anya-e0605n9:20_0": "Kiṁ me jarāmaccumukhe ṭhitassa,",
  "anya-e0605n9:21_0": "Na me vase kāmavase ṭhitassa.",
  "anya-e0605n9:22_0": "53.",
  "anya-e0605n9:23_0": " Kāmena kāmena na sādhyamokkhaṁ,",
  "anya-e0605n9:24_0": "Mānena mānena mamatthi kiñci;",
  "anya-e0605n9:25_0": "Māro saseno hi avāraṇīyo,",
  "anya-e0605n9:26_0": "Yantena ucchuṁ viya maddatī maṁ.",
  "anya-e0605n9:27_0": "54.",
  "anya-e0605n9:28_0": " Ādittamuyātapayātamūnaṁ,",
  "anya-e0605n9:29_0": "Atāṇāleṇāsaraṇe jane te;",
  "anya-e0605n9:30_0": "Disvāna disvāna sivaṁ mayā te,",
  "anya-e0605n9:31_0": "Kāmena kāmena kathaṁ vineyya.",
  "anya-e0605n9:32_0": "55.",
  "anya-e0605n9:33_0": " Vijjāvijjāya cutañcupetaṁ,",
  "anya-e0605n9:34_0": "Asārasārūpagatañjanaṁ janaṁ;",
  "anya-e0605n9:35_0": "Vijjāyavijjāya yuto cutohaṁ,",
  "anya-e0605n9:36_0": "Pahomi tāretumasaṅgaho gato.",
  "anya-e0605n9:37_0": "56.",
  "anya-e0605n9:38_0": " Magganti no diṭṭhigatāpavaggaṁ,",
  "anya-e0605n9:39_0": "Aggā ti tevāhu janā samaggā;",
  "anya-e0605n9:40_0": "Naggaṁ aho mohatamassa vaggaṁ,",
  "anya-e0605n9:41_0": "Vaggaṁ hanissāmi tamaggamaggā.",
  "anya-e0605n9:42_0": "57.",
  "anya-e0605n9:43_0": " Paseyhakārena aseyhadukkhaṁ,",
  "anya-e0605n9:44_0": "Janā janentīha janānameva;",
  "anya-e0605n9:45_0": "Paseyhakārenā aseyhadukkhaṁ,",
  "anya-e0605n9:46_0": "Pāpaṁ na jānanti tato nidānaṁ.",
  "anya-e0605n9:47_0": "58.",
  "anya-e0605n9:48_0": " Te oghayogāsavasaṁkilesā,",
  "anya-e0605n9:49_0": "Tameva nāsenti tato samuṭṭhitā;",
  "anya-e0605n9:50_0": "Ekantikaṁ jāti jarā ca maccu,",
  "anya-e0605n9:51_0": "Nirantaraṁ taṁ byasanañcanekaṁ.",
  "anya-e0605n9:52_0": "59.",
  "anya-e0605n9:53_0": " Cīraṁ kilesānasamujjalantaṁ,",
  "anya-e0605n9:54_0": "Disvāna sattānusayaṁ sayambhū;",
  "anya-e0605n9:55_0": "Sādhemi bodhiṁ vinayāmi satte,",
  "anya-e0605n9:56_0": "Pacchāpi passāmi sutaṁ sutantaṁ.",
  "anya-e0605n9:57_0": "60.",
  "anya-e0605n9:58_0": " Taṁ dibbacakkaṁ khuracakkamālaṁ,",
  "anya-e0605n9:59_0": "Rajjaṁ sasārajjasamajjamajjaṁ;",
  "anya-e0605n9:60_0": "Te bandhavā bandhanamāgatā pare,",
  "anya-e0605n9:61_0": "Suto pasūtoyamanaṅgadūto.",
  "anya-e0605n9:62_0": "61.",
  "anya-e0605n9:63_0": " Samujjalantaṁ vasatī satīsirī,",
  "anya-e0605n9:64_0": "Sirīsapāgāramidaṁ mahāvisaṁ;",
  "anya-e0605n9:65_0": "Daddallamānā yuvatī vatīmā,",
  "anya-e0605n9:66_0": "Sakaṇṭakāyeva samañjasañjase.",
  "anya-e0605n9:67_0": "62.",
  "anya-e0605n9:68_0": " Yassā virājitasirī siriyāpi natthi,",
  "anya-e0605n9:69_0": "Tassāvalokiya na tittivasānamatthi;",
  "anya-e0605n9:70_0": "Gacchāmi handa tavanaṅga sirappabhedaṁ,",
  "anya-e0605n9:71_0": "Mattebhakumbhupari sīhavilāsagāmiṁ.",
  "anya-e0605n9:72_0": "63.",
  "anya-e0605n9:73_0": " Bho bho anaṅgasucira pi panuṇṇabāṇa,",
  "anya-e0605n9:74_0": "Bāṇāni saṁhara panuṇṇamito nirodha;",
  "anya-e0605n9:75_0": "Rodhena cāpadagato manaso na soca,",
  "anya-e0605n9:76_0": "Socaṁ tavappanavalokiya yāmi santiṁ.",
  "anya-e0605n9:77_0": "64.",
  "anya-e0605n9:78_0": " Ratī ratī kāmaguṇe viveke,",
  "anya-e0605n9:79_0": "Alaṁ alanteva vicintayanto;",
  "anya-e0605n9:80_0": "Manaṁ manaṅgālayasampadālayaṁ,",
  "anya-e0605n9:81_0": "Tahiṁ tahiṁ diṭṭhabālā va pakkami."
}