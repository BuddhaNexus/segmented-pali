{
  "ja166:0.1_0": "Jātaka",
  "ja166:0.2_0": "Dukanipāta",
  "ja166:0.3_0": "Santhavavagga",
  "ja166:0.4_0": "6. Upasāḷakajātaka",
  "ja166:1.1_0": "“Upasāḷakanāmāni,",
  "ja166:1.2_0": "sahassāni catuddasa;",
  "ja166:1.3_0": "Asmiṁ padese daḍḍhāni,",
  "ja166:1.4_0": "natthi loke anāmataṁ.",
  "ja166:2.1_0": "Yamhi saccañca dhammo ca,",
  "ja166:2.2_0": "ahiṁsā saṁyamo damo;",
  "ja166:2.3_0": "Etaṁ ariyā sevanti,",
  "ja166:2.4_0": "etaṁ loke anāmatan”ti.",
  "ja166:3.1_0": "Upasāḷakajātakaṁ chaṭṭhaṁ."
}