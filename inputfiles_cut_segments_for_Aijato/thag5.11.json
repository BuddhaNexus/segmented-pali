{
  "thag5.11:0.1_0": "Theragāthā",
  "thag5.11:0.2_0": "Pañcakanipāta",
  "thag5.11:0.3_0": "Paṭhamavagga",
  "thag5.11:0.4_0": "5.11. Soṇakuṭikaṇṇattheragāthā",
  "thag5.11:1.1_0": "“Upasampadā ca me laddhā,",
  "thag5.11:1.2_0": "vimutto camhi anāsavo;",
  "thag5.11:1.3_0": "So ca me bhagavā diṭṭho,",
  "thag5.11:1.4_0": "vihāre ca sahāvasiṁ.",
  "thag5.11:2.1_0": "Bahudeva rattiṁ bhagavā,",
  "thag5.11:2.2_0": "abbhokāsetināmayi;",
  "thag5.11:2.3_0": "Vihārakusalo satthā,",
  "thag5.11:2.4_0": "vihāraṁ pāvisī tadā.",
  "thag5.11:3.1_0": "Santharitvāna saṅghāṭiṁ,",
  "thag5.11:3.2_0": "seyyaṁ kappesi gotamo;",
  "thag5.11:3.3_0": "Sīho selaguhāyaṁva,",
  "thag5.11:3.4_0": "pahīnabhayabheravo.",
  "thag5.11:4.1_0": "Tato kalyāṇavākkaraṇo,",
  "thag5.11:4.2_0": "sammāsambuddhasāvako;",
  "thag5.11:4.3_0": "Soṇo abhāsi saddhammaṁ,",
  "thag5.11:4.4_0": "buddhaseṭṭhassa sammukhā.",
  "thag5.11:5.1_0": "Pañcakkhandhe pariññāya,",
  "thag5.11:5.2_0": "Bhāvayitvāna añjasaṁ;",
  "thag5.11:5.3_0": "Pappuyya paramaṁ santiṁ,",
  "thag5.11:5.4_0": "Parinibbissatyanāsavo”ti.",
  "thag5.11:5.5_0": "… Soṇo kuṭikaṇṇatthero …."
}