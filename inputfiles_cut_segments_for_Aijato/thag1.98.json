{
  "thag1.98:0.1_0": "Theragāthā",
  "thag1.98:0.2_0": "Ekakanipāta",
  "thag1.98:0.3_0": "Dasamavagga",
  "thag1.98:0.4_0": "1.98. Abhayattheragāthā",
  "thag1.98:1.1_0": "“Rūpaṁ disvā sati muṭṭhā,",
  "thag1.98:1.2_0": "Piyaṁ nimittaṁ manasikaroto;",
  "thag1.98:1.3_0": "Sārattacitto vedeti,",
  "thag1.98:1.4_0": "Tañca ajjhosa tiṭṭhati;",
  "thag1.98:1.5_0": "Tassa vaḍḍhanti āsavā,",
  "thag1.98:1.6_0": "Bhavamūlopagāmino”ti.",
  "thag1.98:1.7_0": "… Abhayo thero …."
}