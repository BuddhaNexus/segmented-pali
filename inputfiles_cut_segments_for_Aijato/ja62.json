{
  "ja62:0.1_0": "Jātaka",
  "ja62:0.2_0": "Ekakanipāta",
  "ja62:0.3_0": "Itthivagga",
  "ja62:0.4_0": "2. Aṇḍabhūtajātaka",
  "ja62:1.1_0": "“Yaṁ brāhmaṇo avādesi,",
  "ja62:1.2_0": "vīṇaṁ samukhaveṭhito;",
  "ja62:1.3_0": "Aṇḍabhūtā bhatā bhariyā,",
  "ja62:1.4_0": "tāsu ko jātu vissase”ti.",
  "ja62:2.1_0": "Aṇḍabhūtajātakaṁ dutiyaṁ."
}