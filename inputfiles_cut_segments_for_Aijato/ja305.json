{
  "ja305:0.1_0": "Jātaka",
  "ja305:0.2_0": "Catukkanipāta",
  "ja305:0.3_0": "Kāliṅgavagga",
  "ja305:0.4_0": "5. Sīlavīmaṁsanajātaka",
  "ja305:1.1_0": "“Natthi loke raho nāma,",
  "ja305:1.2_0": "pāpakammaṁ pakubbato;",
  "ja305:1.3_0": "Passanti vanabhūtāni,",
  "ja305:1.4_0": "taṁ bālo maññatī raho.",
  "ja305:2.1_0": "Ahaṁ raho na passāmi,",
  "ja305:2.2_0": "suññaṁ vāpi na vijjati;",
  "ja305:2.3_0": "Yattha aññaṁ na passāmi,",
  "ja305:2.4_0": "asuññaṁ hoti taṁ mayā”.",
  "ja305:3.1_0": "“Dujjacco ca sujacco ca,",
  "ja305:3.2_0": "nando ca sukhavaḍḍhito;",
  "ja305:3.3_0": "Vejjo ca addhuvasīlo ca,",
  "ja305:3.4_0": "te dhammaṁ jahu matthikā.",
  "ja305:4.1_0": "Brāhmaṇo ca kathaṁ jahe,",
  "ja305:4.2_0": "sabbadhammāna pāragū;",
  "ja305:4.3_0": "Yo dhammamanupāleti,",
  "ja305:4.4_0": "dhitimā saccanikkamo”ti.",
  "ja305:5.1_0": "Sīlavīmaṁsanajātakaṁ pañcamaṁ."
}