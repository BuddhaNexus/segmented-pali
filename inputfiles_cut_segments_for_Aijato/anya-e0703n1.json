{
  "anya-e0703n1:0_0": "Dutiyapariccheda",
  "anya-e0703n1:1_0": "Mahāsammatavaṁsa",
  "anya-e0703n1:2_0": "1.",
  "anya-e0703n1:3_0": " Mahāsammata rājassa, vaṁsajo hi mahāmuni;",
  "anya-e0703n1:4_0": "Kappassādimhi rājā’si, mahāsammatanāmako.",
  "anya-e0703n1:5_0": "2.",
  "anya-e0703n1:6_0": " Rojo ca vararojo ca, tathā kalyāṇakā duve;",
  "anya-e0703n1:7_0": "Uposatho ca mandhātā, carako’pacarā duve.",
  "anya-e0703n1:8_0": "3.",
  "anya-e0703n1:9_0": " Cetīyo mucalo ceva, mahāmūcalanāmako;",
  "anya-e0703n1:10_0": "Mucalindo sāgaro ceva, sāgaro deva vanāmako.",
  "anya-e0703n1:11_0": "4.",
  "anya-e0703n1:12_0": " Bharato bhagīratho ceva, ruci ca suruci pica;",
  "anya-e0703n1:13_0": "Patāpo mahāpatāpo, panādā ca tathā duve.",
  "anya-e0703n1:14_0": "5.",
  "anya-e0703n1:15_0": " Sudassano ca neru ca, tathā eva duve duve;",
  "anya-e0703n1:16_0": "Pacchimā cā’ti rājāno, tassa puttapaputtakā.",
  "anya-e0703n1:17_0": "6.",
  "anya-e0703n1:18_0": " Asaṁkhiyāyukā ete, aṭṭhavīsati bhūmipā;",
  "anya-e0703n1:19_0": "Kusāvatiṁ rājagahaṁ, mithilañcāpi āvasuṁ.",
  "anya-e0703n1:20_0": "7.",
  "anya-e0703n1:21_0": " Tato satañca rājano, chappaññāsa ca saṭṭhi ca;",
  "anya-e0703n1:22_0": "Caturāsīti sahassāni, chattiṁsā ca tato pare.",
  "anya-e0703n1:23_0": "8.",
  "anya-e0703n1:24_0": " Dvattiṁsa aṭṭhavīsā ca, dvāvīsati tato pare;",
  "anya-e0703n1:25_0": "Aṭṭhārasa sattarasa, pañcadasa catuddasa.",
  "anya-e0703n1:26_0": "9.",
  "anya-e0703n1:27_0": " Nava satta dvādasa ca, pañcavīsa tatopare;",
  "anya-e0703n1:28_0": "Pañcavīsaṁ dvādasa ca, dvādasañca navā pica.",
  "anya-e0703n1:29_0": "10.",
  "anya-e0703n1:30_0": " Caturāsītisahassāni, makhādevādikāpi ca;",
  "anya-e0703n1:31_0": "Caturāsītisahassāni, kaḷārājanakādayo.",
  "anya-e0703n1:32_0": "11.",
  "anya-e0703n1:33_0": " Soḷasa yāva okkātaṁ, paputtā rāsito ime;",
  "anya-e0703n1:34_0": "Visuṁ visuṁ pure rajjaṁ, kamato anusāsisuṁ.",
  "anya-e0703n1:35_0": "12.",
  "anya-e0703n1:36_0": " Okkāmukho jeṭṭhaputto, okkākasā’si bhūpati;",
  "anya-e0703n1:37_0": "Nipuro candīmā candaṁ-mukho ca sivi sañjayo.",
  "anya-e0703n1:38_0": "13.",
  "anya-e0703n1:39_0": " Vessantara mahārājā, jālī ca sīhavāhano;",
  "anya-e0703n1:40_0": "Sīhassaro ca iccete, tassa putta pa puttakā.",
  "anya-e0703n1:41_0": "14.",
  "anya-e0703n1:42_0": " Dveasīti sahassāni, sīhassarassa rājino;",
  "anya-e0703n1:43_0": "Putta pa putta rājāno, jayaseno tadantimo.",
  "anya-e0703n1:44_0": "15.",
  "anya-e0703n1:45_0": " Ete kapilavatthusmiṁ, sakyarājāti vissutā;",
  "anya-e0703n1:46_0": "Sīhahanu mahārājā, jayasenassa atrajo.",
  "anya-e0703n1:47_0": "16.",
  "anya-e0703n1:48_0": " Jayasenassa dhītā ca, nāmenā’si yasodharā;",
  "anya-e0703n1:49_0": "Devadaye devadaha-sakko nāmā’si bhūpati.",
  "anya-e0703n1:50_0": "17.",
  "anya-e0703n1:51_0": " Añjano cā’tha kaccānā, āsuṁ tassa sutā duve;",
  "anya-e0703n1:52_0": "Mahesīcā’si kaccānā, rañño sīhahanussa sā.",
  "anya-e0703n1:53_0": "18.",
  "anya-e0703n1:54_0": " Āsī añjanasakkassa, mahesī sā yasodharā;",
  "anya-e0703n1:55_0": "Añjanassa duve dhītā, māyā cātha pajāpati.",
  "anya-e0703n1:56_0": "19.",
  "anya-e0703n1:57_0": " Puttā duve daṇḍapāṇī, suppabuddho ca sākiyo;",
  "anya-e0703n1:58_0": "Pañca puttā duve dhītā, āsuṁ sīhahanussare.",
  "anya-e0703n1:59_0": "20.",
  "anya-e0703n1:60_0": " Suddhodano dhotodano, sakkasukkamitodano;",
  "anya-e0703n1:61_0": "Amitā pamitācā’ti, ime pañca imā duve.",
  "anya-e0703n1:62_0": "21.",
  "anya-e0703n1:63_0": " Suppabuddhassa sakkassa, mahesī amitā ahu;",
  "anya-e0703n1:64_0": "Tassā’suṁ bhaddakaccānā, devadatto duve sutā.",
  "anya-e0703n1:65_0": "22.",
  "anya-e0703n1:66_0": " Māyā mahāpajāpati ceva, suddhodana mahesīyo;",
  "anya-e0703n1:67_0": "Suddhodana mahārañño, putto māyāya so jino.",
  "anya-e0703n1:68_0": "23.",
  "anya-e0703n1:69_0": " Mahā sammatavaṁsamhi, asambhinne mahāmuni;",
  "anya-e0703n1:70_0": "Evaṁ pavatte sañjāto, sabba khatthiya muddhani.",
  "anya-e0703n1:71_0": "24.",
  "anya-e0703n1:72_0": " Siddhatthassa kumārassa, bodhisattassa sā ahu;",
  "anya-e0703n1:73_0": "Mahesī bhaddakaccānā, putto tassā’si rāhulo.",
  "anya-e0703n1:74_0": "25.",
  "anya-e0703n1:75_0": " Bimbisāro ca siddhattha-kumāro ca sahāyakā;",
  "anya-e0703n1:76_0": "Ubhinnaṁ pitaro cāpi, sahāyāeva te ahuṁ.",
  "anya-e0703n1:77_0": "26.",
  "anya-e0703n1:78_0": " Bodhisatto bimbisārā, pañcavassādhiko ahu;",
  "anya-e0703n1:79_0": "Ekūnatiṁso vayasā, bodhisatto’bhinikkhami.",
  "anya-e0703n1:80_0": "27.",
  "anya-e0703n1:81_0": " Padahitvāna chabbassaṁ, bodhiṁ patvā kamena ca;",
  "anya-e0703n1:82_0": "Pañcatiṁso tha vayasā, bimbisāramupāgami.",
  "anya-e0703n1:83_0": "28.",
  "anya-e0703n1:84_0": " Bimbisāro pannarasa-vasso’tha pītaraṁ sayaṁ;",
  "anya-e0703n1:85_0": "Abhisitto mahāpañño, patto rajjassa tassa tu.",
  "anya-e0703n1:86_0": "29.",
  "anya-e0703n1:87_0": " Patte soḷasame vasse, satthā dhammamadesayi;",
  "anya-e0703n1:88_0": "Dvāpaññāseva vassāni, rajjaṁ kāresi so pana.",
  "anya-e0703n1:89_0": "30.",
  "anya-e0703n1:90_0": " Rajje samā pannarasa, pubbe jinasamāgamā;",
  "anya-e0703n1:91_0": "Sattatiṁsa samā tassa, dharamāne tathāgate.",
  "anya-e0703n1:92_0": "31.",
  "anya-e0703n1:93_0": " Bimbisārasuto’jāta-sattutaṁ ghātīyā’mati;",
  "anya-e0703n1:94_0": "Rajjaṁ dvattiṁsavassāni, mahāmittaddukārayī.",
  "anya-e0703n1:95_0": "32.",
  "anya-e0703n1:96_0": " Ajātasattuno vasse, aṭṭhame muni nibbuto;",
  "anya-e0703n1:97_0": "Pacchā so kārayī rajjaṁ, vassāni catuvīsati.",
  "anya-e0703n1:98_0": "33.",
  "anya-e0703n1:99_0": " Tathāgato sakalalokaggataṁ gato;",
  "anya-e0703n1:100_0": "Aniccatāva samavaso upāgato;",
  "anya-e0703n1:101_0": "Iti’dha yo bhayajananiṁ aniccataṁ,",
  "anya-e0703n1:102_0": "Avekkhate sa bhavati dukkhapāragūti.",
  "anya-e0703n1:103_0": "Sujanappasādasaṁvegatthāya kate mahāvaṁse",
  "anya-e0703n1:104_0": "Mahāsammatavaṁso nāma",
  "anya-e0703n1:105_0": "Dutiyo paricchedo."
}