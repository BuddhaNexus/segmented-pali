{
  "ja200:0.1_0": "Jātaka",
  "ja200:0.2_0": "Dukanipāta",
  "ja200:0.3_0": "Ruhakavagga",
  "ja200:0.4_0": "10. Sādhusīlajātaka",
  "ja200:1.1_0": "“Sarīradabyaṁ vuḍḍhabyaṁ,",
  "ja200:1.2_0": "sojaccaṁ sādhusīliyaṁ;",
  "ja200:1.3_0": "Brāhmaṇaṁ teva pucchāma,",
  "ja200:1.4_0": "kaṁ nu tesaṁ vanimhase”.",
  "ja200:2.1_0": "“Attho atthi sarīrasmiṁ,",
  "ja200:2.2_0": "vuḍḍhabyassa namo kare;",
  "ja200:2.3_0": "Attho atthi sujātasmiṁ,",
  "ja200:2.4_0": "sīlaṁ asmāka ruccatī”ti.",
  "ja200:3.1_0": "Sādhusīlajātakaṁ dasamaṁ.",
  "ja200:3.2_0": "Ruhakavaggo pañcamo.",
  "ja200:4.1_0": "Tassuddānaṁ",
  "ja200:5.1_0": "Apiruhaka rūpavatī musalo,",
  "ja200:5.2_0": "Pavasanti sapañcamapokkharaṇī;",
  "ja200:5.3_0": "Atha muttimavāṇija umhayate,",
  "ja200:5.4_0": "Ciraāgata koṭṭha sarīra dasāti."
}