{
  "tha-ap245:0.1_0": "Therāpadāna",
  "tha-ap245:0.2_0": "Tuvaradāyakavagga",
  "tha-ap245:0.3_0": "3. Naḷinakesariyattheraapadāna",
  "tha-ap245:1.1_0": "“Jātassarassa vemajjhe,",
  "tha-ap245:1.2_0": "vasāmi jalakukkuṭo;",
  "tha-ap245:1.3_0": "Addasāhaṁ devadevaṁ,",
  "tha-ap245:1.4_0": "gacchantaṁ anilañjase.",
  "tha-ap245:2.1_0": "Tuṇḍena kesariṁ gayha,",
  "tha-ap245:2.2_0": "vippasannena cetasā;",
  "tha-ap245:2.3_0": "Buddhassa abhiropesiṁ,",
  "tha-ap245:2.4_0": "tissassa lokabandhuno.",
  "tha-ap245:3.1_0": "Dvenavute ito kappe,",
  "tha-ap245:3.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap245:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap245:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap245:4.1_0": "Tesattatimhi kappamhi,",
  "tha-ap245:4.2_0": "satta kesaranāmakā;",
  "tha-ap245:4.3_0": "Sattaratanasampannā,",
  "tha-ap245:4.4_0": "cakkavattī mahabbalā.",
  "tha-ap245:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap245:5.2_0": "…pe…",
  "tha-ap245:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap245:6.1_0": "Itthaṁ sudaṁ āyasmā naḷinakesariyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap245:7.1_0": "Naḷinakesariyattherassāpadānaṁ tatiyaṁ."
}