{
  "ja397:0.1_0": "Jātaka",
  "ja397:0.2_0": "Sattakanipāta",
  "ja397:0.3_0": "Kukkuvagga",
  "ja397:0.4_0": "2. Manojajātaka",
  "ja397:1.1_0": "“Yathā cāpo ninnamati,",
  "ja397:1.2_0": "jiyā cāpi nikūjati;",
  "ja397:1.3_0": "Haññate nūna manojo,",
  "ja397:1.4_0": "migarājā sakhā mama.",
  "ja397:2.1_0": "Handa dāni vanantāni,",
  "ja397:2.2_0": "pakkamāmi yathāsukhaṁ;",
  "ja397:2.3_0": "Netādisā sakhā honti,",
  "ja397:2.4_0": "labbhā me jīvato sakhā”.",
  "ja397:3.1_0": "“Na pāpajanasaṁsevī,",
  "ja397:3.2_0": "accantaṁ sukhamedhati;",
  "ja397:3.3_0": "Manojaṁ passa semānaṁ,",
  "ja397:3.4_0": "giriyassānusāsanī”.",
  "ja397:4.1_0": "“Na pāpasampavaṅkena,",
  "ja397:4.2_0": "mātā puttena nandati;",
  "ja397:4.3_0": "Manojaṁ passa semānaṁ,",
  "ja397:4.4_0": "acchannaṁ samhi lohite”.",
  "ja397:5.1_0": "“Evamāpajjate poso,",
  "ja397:5.2_0": "pāpiyo ca nigacchati;",
  "ja397:5.3_0": "Yo ve hitānaṁ vacanaṁ,",
  "ja397:5.4_0": "na karoti atthadassinaṁ”.",
  "ja397:6.1_0": "“Evañca so hoti tato ca pāpiyo,",
  "ja397:6.2_0": "Yo uttamo adhamajanūpasevī;",
  "ja397:6.3_0": "Passuttamaṁ adhamajanūpasevitaṁ,",
  "ja397:6.4_0": "Migādhipaṁ saravaraveganiddhutaṁ”.",
  "ja397:7.1_0": "“Nihīyati puriso nihīnasevī,",
  "ja397:7.2_0": "Na ca hāyetha kadāci tulyasevī;",
  "ja397:7.3_0": "Seṭṭhamupagamaṁ udeti khippaṁ,",
  "ja397:7.4_0": "Tasmāttanā uttaritaraṁ bhajethā”ti.",
  "ja397:8.1_0": "Manojajātakaṁ dutiyaṁ."
}