{
  "tha-ap455:0.1_0": "Therāpadāna",
  "tha-ap455:0.2_0": "Jagatidāyakavagga",
  "tha-ap455:0.3_0": "3 Sīhāsanabījiyattheraapadāna",
  "tha-ap455:1.1_0": "“Tissassāhaṁ bhagavato,",
  "tha-ap455:1.2_0": "bodhirukkhamavandiyaṁ;",
  "tha-ap455:1.3_0": "Paggayha bījaniṁ tattha,",
  "tha-ap455:1.4_0": "sīhāsanamabījahaṁ.",
  "tha-ap455:2.1_0": "Dvenavute ito kappe,",
  "tha-ap455:2.2_0": "sīhāsanamabījahaṁ;",
  "tha-ap455:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap455:2.4_0": "bījanāya idaṁ phalaṁ.",
  "tha-ap455:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap455:3.2_0": "…pe…",
  "tha-ap455:3.3_0": "viharāmi anāsavo.",
  "tha-ap455:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap455:4.2_0": "…pe…",
  "tha-ap455:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap455:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap455:5.2_0": "…pe…",
  "tha-ap455:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap455:6.1_0": "Itthaṁ sudaṁ āyasmā sīhāsanabījiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap455:7.1_0": "Sīhāsanabījiyattherassāpadānaṁ tatiyaṁ."
}