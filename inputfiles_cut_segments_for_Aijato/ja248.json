{
  "ja248:0.1_0": "Jātaka",
  "ja248:0.2_0": "Dukanipāta",
  "ja248:0.3_0": "Siṅgālavagga",
  "ja248:0.4_0": "8. Kiṁsukopamajātaka",
  "ja248:1.1_0": "“Sabbehi kiṁsuko diṭṭho,",
  "ja248:1.2_0": "kinnvettha vicikicchatha;",
  "ja248:1.3_0": "Na hi sabbesu ṭhānesu,",
  "ja248:1.4_0": "sārathī paripucchito”.",
  "ja248:2.1_0": "“Evaṁ sabbehi ñāṇehi,",
  "ja248:2.2_0": "yesaṁ dhammā ajānitā;",
  "ja248:2.3_0": "Te ve dhammesu kaṅkhanti,",
  "ja248:2.4_0": "kiṁsukasmiṁva bhātaro”ti.",
  "ja248:3.1_0": "Kiṁsukopamajātakaṁ aṭṭhamaṁ."
}