{
  "ja131:0.1_0": "Jātaka",
  "ja131:0.2_0": "Ekakanipāta",
  "ja131:0.3_0": "Asampadānavagga",
  "ja131:0.4_0": "1. Asampadānajātaka",
  "ja131:1.1_0": "“Asampadānenitarītarassa,",
  "ja131:1.2_0": "Bālassa mittāni kalī bhavanti;",
  "ja131:1.3_0": "Tasmā harāmi bhusaṁ aḍḍhamānaṁ,",
  "ja131:1.4_0": "Mā me mitti jīyittha sassatāyan”ti.",
  "ja131:2.1_0": "Asampadānajātakaṁ paṭhamaṁ."
}