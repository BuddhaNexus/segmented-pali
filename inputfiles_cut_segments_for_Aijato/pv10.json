{
  "pv10:0.1_0": "Petavatthu",
  "pv10:0.2_0": "Uragavagga",
  "pv10:0.3_0": "10. Khallāṭiyapetivatthu",
  "pv10:1.1_0": "“Kā nu antovimānasmiṁ,",
  "pv10:1.2_0": "tiṭṭhantī nūpanikkhami;",
  "pv10:1.3_0": "Upanikkhamassu bhadde,",
  "pv10:1.4_0": "passāma taṁ bahiṭṭhitan”ti.",
  "pv10:2.1_0": "“Aṭṭīyāmi harāyāmi,",
  "pv10:2.2_0": "naggā nikkhamituṁ bahi;",
  "pv10:2.3_0": "Kesehamhi paṭicchannā,",
  "pv10:2.4_0": "puññaṁ me appakaṁ katan”ti.",
  "pv10:3.1_0": "“Handuttarīyaṁ dadāmi te,",
  "pv10:3.2_0": "idaṁ dussaṁ nivāsaya;",
  "pv10:3.3_0": "Idaṁ dussaṁ nivāsetvā,",
  "pv10:3.4_0": "ehi nikkhama sobhane;",
  "pv10:3.5_0": "Upanikkhamassu bhadde,",
  "pv10:3.6_0": "passāma taṁ bahiṭṭhitan”ti.",
  "pv10:4.1_0": "“Hatthena hatthe te dinnaṁ,",
  "pv10:4.2_0": "na mayhaṁ upakappati;",
  "pv10:4.3_0": "Esetthupāsako saddho,",
  "pv10:4.4_0": "sammāsambuddhasāvako.",
  "pv10:5.1_0": "Etaṁ acchādayitvāna,",
  "pv10:5.2_0": "mama dakkhiṇamādisa;",
  "pv10:5.3_0": "Tathāhaṁ sukhitā hessaṁ,",
  "pv10:5.4_0": "sabbakāmasamiddhinī”ti.",
  "pv10:6.1_0": "Tañca te nhāpayitvāna,",
  "pv10:6.2_0": "vilimpetvāna vāṇijā;",
  "pv10:6.3_0": "Vatthehacchādayitvāna,",
  "pv10:6.4_0": "tassā dakkhiṇamādisuṁ.",
  "pv10:7.1_0": "Samanantarānuddiṭṭhe,",
  "pv10:7.2_0": "vipāko udapajjatha;",
  "pv10:7.3_0": "Bhojanacchādanapānīyaṁ,",
  "pv10:7.4_0": "dakkhiṇāya idaṁ phalaṁ.",
  "pv10:8.1_0": "Tato suddhā sucivasanā,",
  "pv10:8.2_0": "kāsikuttamadhārinī;",
  "pv10:8.3_0": "Hasantī vimānā nikkhami,",
  "pv10:8.4_0": "dakkhiṇāya idaṁ phalanti.",
  "pv10:9.1_0": "“Sucittarūpaṁ ruciraṁ,",
  "pv10:9.2_0": "vimānaṁ te pabhāsati;",
  "pv10:9.3_0": "Devate pucchitācikkha,",
  "pv10:9.4_0": "kissa kammassidaṁ phalan”ti.",
  "pv10:10.1_0": "“Bhikkhuno caramānassa,",
  "pv10:10.2_0": "doṇinimmajjaniṁ ahaṁ;",
  "pv10:10.3_0": "Adāsiṁ ujubhūtassa,",
  "pv10:10.4_0": "vippasannena cetasā.",
  "pv10:11.1_0": "Tassa kammassa kusalassa,",
  "pv10:11.2_0": "vipākaṁ dīghamantaraṁ;",
  "pv10:11.3_0": "Anubhomi vimānasmiṁ,",
  "pv10:11.4_0": "tañca dāni parittakaṁ.",
  "pv10:12.1_0": "Uddhaṁ catūhi māsehi,",
  "pv10:12.2_0": "kālakiriyā bhavissati;",
  "pv10:12.3_0": "Ekantakaṭukaṁ ghoraṁ,",
  "pv10:12.4_0": "nirayaṁ papatissahaṁ.",
  "pv10:13.1_0": "Catukkaṇṇaṁ catudvāraṁ,",
  "pv10:13.2_0": "vibhattaṁ bhāgaso mitaṁ;",
  "pv10:13.3_0": "Ayopākārapariyantaṁ,",
  "pv10:13.4_0": "ayasā paṭikujjitaṁ.",
  "pv10:14.1_0": "Tassa ayomayā bhūmi,",
  "pv10:14.2_0": "jalitā tejasā yutā;",
  "pv10:14.3_0": "Samantā yojanasataṁ,",
  "pv10:14.4_0": "pharitvā tiṭṭhati sabbadā.",
  "pv10:15.1_0": "Tatthāhaṁ dīghamaddhānaṁ,",
  "pv10:15.2_0": "dukkhaṁ vedissa vedanaṁ;",
  "pv10:15.3_0": "Phalañca pāpakammassa,",
  "pv10:15.4_0": "tasmā socāmahaṁ bhusan”ti.",
  "pv10:16.1_0": "Khallāṭiyapetivatthu dasamaṁ."
}