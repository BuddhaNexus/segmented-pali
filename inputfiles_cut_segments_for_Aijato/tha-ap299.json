{
  "tha-ap299:0.1_0": "Therāpadāna",
  "tha-ap299:0.2_0": "Citakapūjakavagga",
  "tha-ap299:0.3_0": "7 Desakittakattheraapadāna",
  "tha-ap299:1.1_0": "“Upasālakanāmohaṁ,",
  "tha-ap299:1.2_0": "ahosiṁ brāhmaṇo tadā;",
  "tha-ap299:1.3_0": "Kānanaṁ vanamogāḷhaṁ,",
  "tha-ap299:1.4_0": "lokajeṭṭhaṁ narāsabhaṁ.",
  "tha-ap299:2.1_0": "Disvāna vandiṁ pādesu,",
  "tha-ap299:2.2_0": "lokāhutipaṭiggahaṁ;",
  "tha-ap299:2.3_0": "Pasannacittaṁ maṁ ñatvā,",
  "tha-ap299:2.4_0": "buddho antaradhāyatha.",
  "tha-ap299:3.1_0": "Kānanā abhinikkhamma,",
  "tha-ap299:3.2_0": "buddhaseṭṭhamanussariṁ;",
  "tha-ap299:3.3_0": "Taṁ desaṁ kittayitvāna,",
  "tha-ap299:3.4_0": "kappaṁ saggamhi modahaṁ.",
  "tha-ap299:4.1_0": "Dvenavute ito kappe,",
  "tha-ap299:4.2_0": "yaṁ desamabhikittayiṁ;",
  "tha-ap299:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap299:4.4_0": "kittanāya idaṁ phalaṁ.",
  "tha-ap299:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap299:5.2_0": "…pe…",
  "tha-ap299:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap299:6.1_0": "Itthaṁ sudaṁ āyasmā desakittako thero imā gāthāyo abhāsitthāti.",
  "tha-ap299:7.1_0": "Desakittakattherassāpadānaṁ sattamaṁ."
}