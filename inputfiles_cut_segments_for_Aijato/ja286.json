{
  "ja286:0.1_0": "Jātaka",
  "ja286:0.2_0": "Tikanipāta",
  "ja286:0.3_0": "Abbhantaravagga",
  "ja286:0.4_0": "6. Sālūkajātaka",
  "ja286:1.1_0": "“Mā sālūkassa pihayi,",
  "ja286:1.2_0": "āturannāni bhuñjati;",
  "ja286:1.3_0": "Appossukko bhusaṁ khāda,",
  "ja286:1.4_0": "etaṁ dīghāyulakkhaṇaṁ.",
  "ja286:2.1_0": "Idāni so idhāgantvā,",
  "ja286:2.2_0": "atithī yuttasevako;",
  "ja286:2.3_0": "Atha dakkhasi sālūkaṁ,",
  "ja286:2.4_0": "sayantaṁ musaluttaraṁ”.",
  "ja286:3.1_0": "Vikantaṁ sūkaraṁ disvā,",
  "ja286:3.2_0": "sayantaṁ musaluttaraṁ;",
  "ja286:3.3_0": "Jaraggavā vicintesuṁ,",
  "ja286:3.4_0": "varamhākaṁ bhusāmivāti.",
  "ja286:4.1_0": "Sālūkajātakaṁ chaṭṭhaṁ."
}