{
  "ja83:0.1_0": "Jātaka",
  "ja83:0.2_0": "Ekakanipāta",
  "ja83:0.3_0": "Apāyimhavagga",
  "ja83:0.4_0": "3. Kāḷakaṇṇijātaka",
  "ja83:1.1_0": "“Mitto have sattapadena hoti,",
  "ja83:1.2_0": "Sahāyo pana dvādasakena hoti;",
  "ja83:1.3_0": "Māsaḍḍhamāsena ca ñāti hoti,",
  "ja83:1.4_0": "Tatuttariṁ attasamopi hoti;",
  "ja83:1.5_0": "Sohaṁ kathaṁ attasukhassa hetu,",
  "ja83:1.6_0": "Cirasanthutaṁ kāḷakaṇṇiṁ jaheyyan”ti.",
  "ja83:2.1_0": "Kāḷakaṇṇijātakaṁ tatiyaṁ"
}