{
  "thag5.2:0.1_0": "Theragāthā",
  "thag5.2:0.2_0": "Pañcakanipāta",
  "thag5.2:0.3_0": "Paṭhamavagga",
  "thag5.2:0.4_0": "5.2. Subhūtattheragāthā",
  "thag5.2:1.1_0": "“Ayoge yuñjamattānaṁ,",
  "thag5.2:1.2_0": "puriso kiccamicchako;",
  "thag5.2:1.3_0": "Carañce nādhigaccheyya,",
  "thag5.2:1.4_0": "‘taṁ me dubbhagalakkhaṇaṁ’.",
  "thag5.2:2.1_0": "Abbūḷhaṁ aghagataṁ vijitaṁ,",
  "thag5.2:2.2_0": "Ekañce ossajeyya kalīva siyā;",
  "thag5.2:2.3_0": "Sabbānipi ce ossajeyya andhova siyā,",
  "thag5.2:2.4_0": "Samavisamassa adassanato.",
  "thag5.2:3.1_0": "Yañhi kayirā tañhi vade,",
  "thag5.2:3.2_0": "yaṁ na kayirā na taṁ vade;",
  "thag5.2:3.3_0": "Akarontaṁ bhāsamānaṁ,",
  "thag5.2:3.4_0": "parijānanti paṇḍitā.",
  "thag5.2:4.1_0": "Yathāpi ruciraṁ pupphaṁ,",
  "thag5.2:4.2_0": "vaṇṇavantaṁ agandhakaṁ;",
  "thag5.2:4.3_0": "Evaṁ subhāsitā vācā,",
  "thag5.2:4.4_0": "aphalā hoti akubbato.",
  "thag5.2:5.1_0": "Yathāpi ruciraṁ pupphaṁ,",
  "thag5.2:5.2_0": "Vaṇṇavantaṁ sugandhakaṁ;",
  "thag5.2:5.3_0": "Evaṁ subhāsitā vācā,",
  "thag5.2:5.4_0": "Saphalā hoti kubbato”ti.",
  "thag5.2:5.5_0": "… Subhūto thero …."
}