{
  "vv67:0.1_0": "Vimānavatthu",
  "vv67:0.2_0": "Purisavimāna",
  "vv67:0.3_0": "Pāyāsivagga",
  "vv67:0.4_0": "3. Phaladāyakavimānavatthu",
  "vv67:1.1_0": "“Uccamidaṁ maṇithūṇaṁ vimānaṁ,",
  "vv67:1.2_0": "Samantato soḷasa yojanāni;",
  "vv67:1.3_0": "Kūṭāgārā sattasatā uḷārā,",
  "vv67:1.4_0": "Veḷuriyathambhā rucakatthatā subhā.",
  "vv67:2.1_0": "Tatthacchasi pivasi khādasi ca,",
  "vv67:2.2_0": "Dibbā ca vīṇā pavadanti vagguṁ;",
  "vv67:2.3_0": "Aṭṭhaṭṭhakā sikkhitā sādhurūpā,",
  "vv67:2.4_0": "Dibbā ca kaññā tidasacarā uḷārā;",
  "vv67:2.5_0": "Naccanti gāyanti pamodayanti.",
  "vv67:3.1_0": "Deviddhipattosi mahānubhāvo,",
  "vv67:3.2_0": "Manussabhūto kimakāsi puññaṁ;",
  "vv67:3.3_0": "Kenāsi evaṁ jalitānubhāvo,",
  "vv67:3.4_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv67:4.1_0": "So devaputto attamano,",
  "vv67:4.2_0": "…pe…",
  "vv67:4.3_0": "Yassa kammassidaṁ phalaṁ.",
  "vv67:5.1_0": "“Phaladāyī phalaṁ vipulaṁ labhati,",
  "vv67:5.2_0": "Dadamujugatesu pasannamānaso;",
  "vv67:5.3_0": "So hi pamodati saggagato tidive,",
  "vv67:5.4_0": "Anubhoti ca puññaphalaṁ vipulaṁ.",
  "vv67:6.1_0": "Tavevāhaṁ mahāmuni,",
  "vv67:6.2_0": "Adāsiṁ caturo phale.",
  "vv67:7.1_0": "Tasmā hi phalaṁ alameva dātuṁ,",
  "vv67:7.2_0": "Niccaṁ manussena sukhatthikena;",
  "vv67:7.3_0": "Dibbāni vā patthayatā sukhāni,",
  "vv67:7.4_0": "Manussasobhaggatamicchatā vā.",
  "vv67:8.1_0": "Tena metādiso vaṇṇo,",
  "vv67:8.2_0": "…pe…",
  "vv67:8.3_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv67:9.1_0": "Phaladāyakavimānaṁ tatiyaṁ."
}