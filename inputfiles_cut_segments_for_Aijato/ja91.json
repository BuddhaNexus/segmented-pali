{
  "ja91:0.1_0": "Jātaka",
  "ja91:0.2_0": "Ekakanipāta",
  "ja91:0.3_0": "Littavagga",
  "ja91:0.4_0": "1. Littajātaka",
  "ja91:1.1_0": "“Littaṁ paramena tejasā,",
  "ja91:1.2_0": "Gilamakkhaṁ puriso na bujjhati;",
  "ja91:1.3_0": "Gila re gila pāpadhuttaka,",
  "ja91:1.4_0": "Pacchā te kaṭukaṁ bhavissatī”ti.",
  "ja91:2.1_0": "Littajātakaṁ paṭhamaṁ."
}