{
  "snp2.3:0.1_0": "Sutta Nipāta 2.3",
  "snp2.3:0.2_0": "Hirisutta",
  "snp2.3:1.1_0": "Hiriṁ tarantaṁ vijigucchamānaṁ,",
  "snp2.3:1.2_0": "Tavāhamasmi iti bhāsamānaṁ;",
  "snp2.3:1.3_0": "Sayhāni kammāni anādiyantaṁ,",
  "snp2.3:1.4_0": "Neso mamanti iti naṁ vijaññā.",
  "snp2.3:2.1_0": "Ananvayaṁ piyaṁ vācaṁ,",
  "snp2.3:2.2_0": "yo mittesu pakubbati;",
  "snp2.3:2.3_0": "Akarontaṁ bhāsamānaṁ,",
  "snp2.3:2.4_0": "parijānanti paṇḍitā.",
  "snp2.3:3.1_0": "Na so mitto yo sadā appamatto,",
  "snp2.3:3.2_0": "Bhedāsaṅkī randhamevānupassī;",
  "snp2.3:3.3_0": "Yasmiñca seti urasīva putto,",
  "snp2.3:3.4_0": "Sa ve mitto yo parehi abhejjo.",
  "snp2.3:4.1_0": "Pāmujjakaraṇaṁ ṭhānaṁ,",
  "snp2.3:4.2_0": "pasaṁsāvahanaṁ sukhaṁ;",
  "snp2.3:4.3_0": "Phalānisaṁso bhāveti,",
  "snp2.3:4.4_0": "vahanto porisaṁ dhuraṁ.",
  "snp2.3:5.1_0": "Pavivekarasaṁ pitvā,",
  "snp2.3:5.2_0": "Rasaṁ upasamassa ca;",
  "snp2.3:5.3_0": "Niddaro hoti nippāpo,",
  "snp2.3:5.4_0": "Dhammapītirasaṁ pivanti.",
  "snp2.3:5.5_0": "Hirisuttaṁ tatiyaṁ."
}