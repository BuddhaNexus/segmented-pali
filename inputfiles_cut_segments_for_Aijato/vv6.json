{
  "vv6:0.1_0": "Vimānavatthu",
  "vv6:0.2_0": "Itthivimāna",
  "vv6:0.3_0": "Pīṭhavagga",
  "vv6:0.4_0": "6. Paṭhamanāvāvimānavatthu",
  "vv6:1.1_0": "“Suvaṇṇacchadanaṁ nāvaṁ,",
  "vv6:1.2_0": "Nāri āruyha tiṭṭhasi;",
  "vv6:1.3_0": "Ogāhasi pokkharaṇiṁ,",
  "vv6:1.4_0": "Padmaṁ chindasi pāṇinā.",
  "vv6:2.1_0": "Kena tetādiso vaṇṇo,",
  "vv6:2.2_0": "kena te idha mijjhati;",
  "vv6:2.3_0": "Uppajjanti ca te bhogā,",
  "vv6:2.4_0": "ye keci manaso piyā.",
  "vv6:3.1_0": "Pucchāmi taṁ devi mahānubhāve,",
  "vv6:3.2_0": "Manussabhūtā kimakāsi puññaṁ;",
  "vv6:3.3_0": "Kenāsi evaṁ jalitānubhāvā,",
  "vv6:3.4_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv6:4.1_0": "Sā devatā attamanā,",
  "vv6:4.2_0": "moggallānena pucchitā;",
  "vv6:4.3_0": "Pañhaṁ puṭṭhā viyākāsi,",
  "vv6:4.4_0": "yassa kammassidaṁ phalaṁ.",
  "vv6:5.1_0": "“Ahaṁ manussesu manussabhūtā,",
  "vv6:5.2_0": "Purimāya jātiyā manussaloke;",
  "vv6:5.3_0": "Disvāna bhikkhū tasite kilante,",
  "vv6:5.4_0": "Uṭṭhāya pātuṁ udakaṁ adāsiṁ.",
  "vv6:6.1_0": "Yo ve kilantāna pipāsitānaṁ,",
  "vv6:6.2_0": "Uṭṭhāya pātuṁ udakaṁ dadāti;",
  "vv6:6.3_0": "Sītodakā tassa bhavanti najjo,",
  "vv6:6.4_0": "Pahūtamalyā bahupuṇḍarīkā.",
  "vv6:7.1_0": "Taṁ āpagā anupariyanti sabbadā,",
  "vv6:7.2_0": "Sītodakā vālukasanthatā nadī;",
  "vv6:7.3_0": "Ambā ca sālā tilakā ca jambuyo,",
  "vv6:7.4_0": "Uddālakā pāṭaliyo ca phullā.",
  "vv6:8.1_0": "Taṁ bhūmibhāgehi upetarūpaṁ,",
  "vv6:8.2_0": "Vimānaseṭṭhaṁ bhusa sobhamānaṁ;",
  "vv6:8.3_0": "Tassīdha kammassa ayaṁ vipāko,",
  "vv6:8.4_0": "Etādisaṁ puññakatā labhanti.",
  "vv6:9.1_0": "Tena metādiso vaṇṇo,",
  "vv6:9.2_0": "tena me idha mijjhati;",
  "vv6:9.3_0": "Uppajjanti ca me bhogā,",
  "vv6:9.4_0": "ye keci manaso piyā.",
  "vv6:10.1_0": "Akkhāmi te bhikkhu mahānubhāva,",
  "vv6:10.2_0": "Manussabhūtā yamakāsi puññaṁ;",
  "vv6:10.3_0": "Tenamhi evaṁ jalitānubhāvā,",
  "vv6:10.4_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv6:11.1_0": "Paṭhamanāvāvimānaṁ chaṭṭhaṁ."
}