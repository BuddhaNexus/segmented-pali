{
  "ja261:0.1_0": "Jātaka",
  "ja261:0.2_0": "Tikanipāta",
  "ja261:0.3_0": "Padumavagga",
  "ja261:0.4_0": "1. Padumajātaka",
  "ja261:1.1_0": "“Yathā kesā ca massū ca,",
  "ja261:1.2_0": "chinnaṁ chinnaṁ virūhati;",
  "ja261:1.3_0": "Evaṁ rūhatu te nāsā,",
  "ja261:1.4_0": "padumaṁ dehi yācito”.",
  "ja261:2.1_0": "“Yathā sāradikaṁ bījaṁ,",
  "ja261:2.2_0": "khette vuttaṁ virūhati;",
  "ja261:2.3_0": "Evaṁ rūhatu te nāsā,",
  "ja261:2.4_0": "padumaṁ dehi yācito”.",
  "ja261:3.1_0": "“Ubhopi palapantete,",
  "ja261:3.2_0": "api padmāni dassati;",
  "ja261:3.3_0": "Vajjuṁ vā te na vā vajjuṁ,",
  "ja261:3.4_0": "natthi nāsāya rūhanā;",
  "ja261:3.5_0": "Dehi samma padumāni,",
  "ja261:3.6_0": "ahaṁ yācāmi yācito”ti.",
  "ja261:4.1_0": "Padumajātakaṁ paṭhamaṁ."
}