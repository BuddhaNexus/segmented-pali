{
  "ja263:0.1_0": "Jātaka",
  "ja263:0.2_0": "Tikanipāta",
  "ja263:0.3_0": "Padumavagga",
  "ja263:0.4_0": "3. Cūḷapalobhanajātaka",
  "ja263:1.1_0": "“Abhijjamāne vārismiṁ,",
  "ja263:1.2_0": "sayaṁ āgamma iddhiyā;",
  "ja263:1.3_0": "Missībhāvitthiyā gantvā,",
  "ja263:1.4_0": "saṁsīdasi mahaṇṇave.",
  "ja263:2.1_0": "Āvaṭṭanī mahāmāyā,",
  "ja263:2.2_0": "brahmacariyavikopanā;",
  "ja263:2.3_0": "Sīdanti naṁ viditvāna,",
  "ja263:2.4_0": "ārakā parivajjaye.",
  "ja263:3.1_0": "Yaṁ etā upasevanti,",
  "ja263:3.2_0": "chandasā vā dhanena vā;",
  "ja263:3.3_0": "Jātavedova saṁ ṭhānaṁ,",
  "ja263:3.4_0": "khippaṁ anudahanti nan”ti.",
  "ja263:4.1_0": "Cūḷapalobhanajātakaṁ tatiyaṁ."
}