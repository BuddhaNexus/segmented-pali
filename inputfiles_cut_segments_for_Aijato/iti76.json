{
  "iti76:0.1_0": "Itivuttaka 76",
  "iti76:0.2_0": "Tikanipāta",
  "iti76:0.3_0": "Tatiyavagga",
  "iti76:0.4_0": "Sukhapatthanāsutta",
  "iti76:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti76:2.1_0": "“Tīṇimāni, bhikkhave, sukhāni patthayamāno sīlaṁ rakkheyya paṇḍito.",
  "iti76:2.2_0": "Katamāni tīṇi?",
  "iti76:2.3_0": "Pasaṁsā me āgacchatūti sīlaṁ rakkheyya paṇḍito, bhogā me uppajjantūti sīlaṁ rakkheyya paṇḍito, kāyassa bhedā paraṁ maraṇā sugatiṁ saggaṁ lokaṁ upapajjissāmīti sīlaṁ rakkheyya paṇḍito.",
  "iti76:2.4_0": "Imāni kho, bhikkhave, tīṇi sukhāni patthayamāno sīlaṁ rakkheyya paṇḍito”ti.",
  "iti76:2.5_0": "Etamatthaṁ bhagavā avoca.",
  "iti76:2.6_0": "Tatthetaṁ iti vuccati:",
  "iti76:3.1_0": "“Sīlaṁ rakkheyya medhāvī,",
  "iti76:3.2_0": "patthayāno tayo sukhe;",
  "iti76:3.3_0": "Pasaṁsaṁ vittalābhañca,",
  "iti76:3.4_0": "pecca sagge pamodanaṁ.",
  "iti76:4.1_0": "Akarontopi ce pāpaṁ,",
  "iti76:4.2_0": "karontamupasevati;",
  "iti76:4.3_0": "Saṅkiyo hoti pāpasmiṁ,",
  "iti76:4.4_0": "avaṇṇo cassa rūhati.",
  "iti76:5.1_0": "Yādisaṁ kurute mittaṁ,",
  "iti76:5.2_0": "yādisaṁ cūpasevati;",
  "iti76:5.3_0": "Sa ve tādisako hoti,",
  "iti76:5.4_0": "sahavāso hi tādiso.",
  "iti76:6.1_0": "Sevamāno sevamānaṁ,",
  "iti76:6.2_0": "samphuṭṭho samphusaṁ paraṁ;",
  "iti76:6.3_0": "Saro diddho kalāpaṁva,",
  "iti76:6.4_0": "alittamupalimpati;",
  "iti76:6.5_0": "Upalepabhayā dhīro,",
  "iti76:6.6_0": "neva pāpasakhā siyā.",
  "iti76:7.1_0": "Pūtimacchaṁ kusaggena,",
  "iti76:7.2_0": "yo naro upanayhati;",
  "iti76:7.3_0": "Kusāpi pūti vāyanti,",
  "iti76:7.4_0": "evaṁ bālūpasevanā.",
  "iti76:8.1_0": "Tagarañca palāsena,",
  "iti76:8.2_0": "yo naro upanayhati;",
  "iti76:8.3_0": "Pattāpi surabhi vāyanti,",
  "iti76:8.4_0": "evaṁ dhīrūpasevanā.",
  "iti76:9.1_0": "Tasmā pattapuṭasseva,",
  "iti76:9.2_0": "ñatvā sampākamattano;",
  "iti76:9.3_0": "Asante nupaseveyya,",
  "iti76:9.4_0": "sante seveyya paṇḍito;",
  "iti76:9.5_0": "Asanto nirayaṁ nenti,",
  "iti76:9.6_0": "santo pāpenti suggatin”ti.",
  "iti76:10.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti76:10.2_0": "Sattamaṁ."
}