{
  "ja398:0.1_0": "Jātaka",
  "ja398:0.2_0": "Sattakanipāta",
  "ja398:0.3_0": "Kukkuvagga",
  "ja398:0.4_0": "3. Sutanujātaka",
  "ja398:1.1_0": "“Rājā te bhattaṁ pāhesi,",
  "ja398:1.2_0": "Suciṁ maṁsūpasecanaṁ;",
  "ja398:1.3_0": "Maghadevasmiṁ adhivatthe,",
  "ja398:1.4_0": "Ehi nikkhamma bhuñjasu”.",
  "ja398:2.1_0": "“Ehi māṇava orena,",
  "ja398:2.2_0": "bhikkhamādāya sūpitaṁ;",
  "ja398:2.3_0": "Tvañca māṇava bhikkhā ca,",
  "ja398:2.4_0": "ubho bhakkhā bhavissatha”.",
  "ja398:3.1_0": "“Appakena tuvaṁ yakkha,",
  "ja398:3.2_0": "thullamatthaṁ jahissasi;",
  "ja398:3.3_0": "Bhikkhaṁ te nāharissanti,",
  "ja398:3.4_0": "janā maraṇasaññino.",
  "ja398:4.1_0": "Laddhāya yakkhā tava niccabhikkhaṁ,",
  "ja398:4.2_0": "Suciṁ paṇītaṁ rasasā upetaṁ;",
  "ja398:4.3_0": "Bhikkhañca te āhariyo naro idha,",
  "ja398:4.4_0": "Sudullabho hehiti bhakkhite mayi”.",
  "ja398:5.1_0": "“Mameva sutano attho,",
  "ja398:5.2_0": "yathā bhāsasi māṇava;",
  "ja398:5.3_0": "Mayā tvaṁ samanuññāto,",
  "ja398:5.4_0": "sotthiṁ passāhi mātaraṁ.",
  "ja398:6.1_0": "Khaggaṁ chattañca pātiñca,",
  "ja398:6.2_0": "gacchamādāya māṇava;",
  "ja398:6.3_0": "Sotthiṁ passatu te mātā,",
  "ja398:6.4_0": "tvañca passāhi mātaraṁ”.",
  "ja398:7.1_0": "“Evaṁ yakkha sukhī hohi,",
  "ja398:7.2_0": "saha sabbehi ñātibhi;",
  "ja398:7.3_0": "Dhanañca me adhigataṁ,",
  "ja398:7.4_0": "rañño ca vacanaṁ katan”ti.",
  "ja398:8.1_0": "Sutanujātakaṁ tatiyaṁ."
}