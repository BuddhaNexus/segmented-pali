{
  "anya-e0605n13:0_0": "Tipādabyāsayamakagāthā",
  "anya-e0605n13:1_0": "96.",
  "anya-e0605n13:2_0": "Tathāgataccheramahosi tassa,",
  "anya-e0605n13:3_0": "Tathā himāropitadāhasantiṁ;",
  "anya-e0605n13:4_0": "Tathā hi māro pi tadāha santiṁ,",
  "anya-e0605n13:5_0": "Tathā hi māropi tadā hasantiṁ."
}