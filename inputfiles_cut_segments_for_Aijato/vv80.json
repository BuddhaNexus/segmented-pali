{
  "vv80:0.1_0": "Vimānavatthu",
  "vv80:0.2_0": "Purisavimāna",
  "vv80:0.3_0": "Sunikkhittavagga",
  "vv80:0.4_0": "6. Gopālavimānavatthu",
  "vv80:1.1_0": "“Disvāna devaṁ paṭipucchi bhikkhu,",
  "vv80:1.2_0": "Ucce vimānamhi ciraṭṭhitike;",
  "vv80:1.3_0": "Āmuttahatthābharaṇaṁ yasassiṁ,",
  "vv80:1.4_0": "Dibbe vimānamhi yathāpi candimā.",
  "vv80:2.1_0": "Alaṅkato malyadharo suvattho,",
  "vv80:2.2_0": "Sukuṇḍalī kappitakesamassu;",
  "vv80:2.3_0": "Āmuttahatthābharaṇo yasassī,",
  "vv80:2.4_0": "Dibbe vimānamhi yathāpi candimā.",
  "vv80:3.1_0": "Dibbā ca vīṇā pavadanti vagguṁ,",
  "vv80:3.2_0": "Aṭṭhaṭṭhakā sikkhitā sādhurūpā;",
  "vv80:3.3_0": "Dibbā ca kaññā tidasacarā uḷārā,",
  "vv80:3.4_0": "Naccanti gāyanti pamodayanti.",
  "vv80:4.1_0": "Deviddhipattosi mahānubhāvo,",
  "vv80:4.2_0": "Manussabhūto kimakāsi puññaṁ;",
  "vv80:4.3_0": "Kenāsi evaṁ jalitānubhāvo,",
  "vv80:4.4_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv80:5.1_0": "So devaputto attamano,",
  "vv80:5.2_0": "…pe…",
  "vv80:5.3_0": "yassa kammassidaṁ phalaṁ.",
  "vv80:6.1_0": "“Ahaṁ manussesu manussabhūto,",
  "vv80:6.2_0": "Saṅgamma rakkhissaṁ paresaṁ dhenuyo;",
  "vv80:6.3_0": "Tato ca āgā samaṇo mamantike,",
  "vv80:6.4_0": "Gāvo ca māse agamaṁsu khādituṁ.",
  "vv80:7.1_0": "Dvayajja kiccaṁ ubhayañca kāriyaṁ,",
  "vv80:7.2_0": "Iccevahaṁ bhante tadā vicintayiṁ;",
  "vv80:7.3_0": "Tato ca saññaṁ paṭiladdha yoniso,",
  "vv80:7.4_0": "Dadāmi bhanteti khipiṁ anantakaṁ.",
  "vv80:8.1_0": "So māsakhettaṁ turito avāsariṁ,",
  "vv80:8.2_0": "Purā ayaṁ bhañjati yassidaṁ dhanaṁ;",
  "vv80:8.3_0": "Tato ca kaṇho urago mahāviso,",
  "vv80:8.4_0": "Aḍaṁsi pāde turitassa me sato.",
  "vv80:9.1_0": "Svāhaṁ aṭṭomhi dukkhena pīḷito,",
  "vv80:9.2_0": "Bhikkhu ca taṁ sāmaṁ muñcitvānantakaṁ;",
  "vv80:9.3_0": "Ahāsi kummāsaṁ mamānukampayā,",
  "vv80:9.4_0": "Tato cuto kālakatomhi devatā.",
  "vv80:10.1_0": "Tadeva kammaṁ kusalaṁ kataṁ mayā,",
  "vv80:10.2_0": "Sukhañca kammaṁ anubhomi attanā;",
  "vv80:10.3_0": "Tayā hi bhante anukampito bhusaṁ,",
  "vv80:10.4_0": "Kataññutāya abhivādayāmi taṁ.",
  "vv80:11.1_0": "Sadevake loke samārake ca,",
  "vv80:11.2_0": "Añño muni natthi tayānukampako;",
  "vv80:11.3_0": "Tayā hi bhante anukampito bhusaṁ,",
  "vv80:11.4_0": "Kataññutāya abhivādayāmi taṁ.",
  "vv80:12.1_0": "Imasmiṁ loke parasmiṁ vā pana,",
  "vv80:12.2_0": "Añño munī natthi tayānukampako;",
  "vv80:12.3_0": "Tayā hi bhante anukampito bhusaṁ,",
  "vv80:12.4_0": "Kataññutāya abhivādayāmi tan”ti.",
  "vv80:13.1_0": "Gopālavimānaṁ chaṭṭhaṁ."
}