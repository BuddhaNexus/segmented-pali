{
  "ja162:0.1_0": "Jātaka",
  "ja162:0.2_0": "Dukanipāta",
  "ja162:0.3_0": "Santhavavagga",
  "ja162:0.4_0": "2. Santhavajātaka",
  "ja162:1.1_0": "“Na santhavasmā paramatthi pāpiyo,",
  "ja162:1.2_0": "Yo santhavo kāpurisena hoti;",
  "ja162:1.3_0": "Santappito sappinā pāyasena,",
  "ja162:1.4_0": "Kicchākataṁ paṇṇakuṭiṁ adayhi.",
  "ja162:2.1_0": "Na santhavasmā paramatthi seyyo,",
  "ja162:2.2_0": "Yo santhavo sappurisena hoti;",
  "ja162:2.3_0": "Sīhassa byagghassa ca dīpino ca,",
  "ja162:2.4_0": "Sāmā mukhaṁ lehati santhavenā”ti.",
  "ja162:3.1_0": "Santhavajātakaṁ dutiyaṁ."
}