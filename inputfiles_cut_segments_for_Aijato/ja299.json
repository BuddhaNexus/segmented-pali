{
  "ja299:0.1_0": "Jātaka",
  "ja299:0.2_0": "Tikanipāta",
  "ja299:0.3_0": "Kumbhavagga",
  "ja299:0.4_0": "9. Komāraputtajātaka",
  "ja299:1.1_0": "“Pure tuvaṁ sīlavataṁ sakāse,",
  "ja299:1.2_0": "Okkantikaṁ kīḷasi assamamhi;",
  "ja299:1.3_0": "Karohare makkaṭiyāni makkaṭa,",
  "ja299:1.4_0": "Na taṁ mayaṁ sīlavataṁ ramāma”.",
  "ja299:2.1_0": "“Sutā hi mayhaṁ paramā visuddhi,",
  "ja299:2.2_0": "Komāraputtassa bahussutassa;",
  "ja299:2.3_0": "Mā dāni maṁ maññi tuvaṁ yathā pure,",
  "ja299:2.4_0": "Jhānānuyutto viharāmi āvuso”.",
  "ja299:3.1_0": "“Sacepi selasmi vapeyya bījaṁ,",
  "ja299:3.2_0": "Devo ca vasse na hi taṁ virūḷhe;",
  "ja299:3.3_0": "Sutā hi te sā paramā visuddhi,",
  "ja299:3.4_0": "Ārā tuvaṁ makkaṭa jhānabhūmiyā”ti.",
  "ja299:4.1_0": "Komāraputtajātakaṁ navamaṁ."
}