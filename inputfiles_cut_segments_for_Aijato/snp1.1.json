{
  "snp1.1:0.1_0": "Sutta Nipāta 1.1",
  "snp1.1:0.2_0": "Uragasutta",
  "snp1.1:1.1_0": "Yo uppatitaṁ vineti kodhaṁ,",
  "snp1.1:1.2_0": "Visaṭaṁ sappavisaṁva osadhehi;",
  "snp1.1:1.3_0": "So bhikkhu jahāti orapāraṁ,",
  "snp1.1:1.4_0": "Urago jiṇṇamivattacaṁ purāṇaṁ.",
  "snp1.1:2.1_0": "Yo rāgamudacchidā asesaṁ,",
  "snp1.1:2.2_0": "Bhisapupphaṁva saroruhaṁ vigayha;",
  "snp1.1:2.3_0": "So bhikkhu jahāti orapāraṁ,",
  "snp1.1:2.4_0": "Urago jiṇṇamivattacaṁ purāṇaṁ.",
  "snp1.1:3.1_0": "Yo taṇhamudacchidā asesaṁ,",
  "snp1.1:3.2_0": "Saritaṁ sīghasaraṁ visosayitvā;",
  "snp1.1:3.3_0": "So bhikkhu jahāti orapāraṁ,",
  "snp1.1:3.4_0": "Urago jiṇṇamivattacaṁ purāṇaṁ.",
  "snp1.1:4.1_0": "Yo mānamudabbadhī asesaṁ,",
  "snp1.1:4.2_0": "Naḷasetuṁva sudubbalaṁ mahogho;",
  "snp1.1:4.3_0": "So bhikkhu jahāti orapāraṁ,",
  "snp1.1:4.4_0": "Urago jiṇṇamivattacaṁ purāṇaṁ.",
  "snp1.1:5.1_0": "Yo nājjhagamā bhavesu sāraṁ,",
  "snp1.1:5.2_0": "Vicinaṁ pupphamivā udumbaresu;",
  "snp1.1:5.3_0": "So bhikkhu jahāti orapāraṁ,",
  "snp1.1:5.4_0": "Urago jiṇṇamivattacaṁ purāṇaṁ.",
  "snp1.1:6.1_0": "Yassantarato na santi kopā,",
  "snp1.1:6.2_0": "Itibhavābhavatañca vītivatto;",
  "snp1.1:6.3_0": "So bhikkhu jahāti orapāraṁ,",
  "snp1.1:6.4_0": "Urago jiṇṇamivattacaṁ purāṇaṁ.",
  "snp1.1:7.1_0": "Yassa vitakkā vidhūpitā,",
  "snp1.1:7.2_0": "Ajjhattaṁ suvikappitā asesā;",
  "snp1.1:7.3_0": "So bhikkhu jahāti orapāraṁ,",
  "snp1.1:7.4_0": "Urago jiṇṇamivattacaṁ purāṇaṁ.",
  "snp1.1:8.1_0": "Yo nāccasārī na paccasārī,",
  "snp1.1:8.2_0": "Sabbaṁ accagamā imaṁ papañcaṁ;",
  "snp1.1:8.3_0": "So bhikkhu jahāti orapāraṁ,",
  "snp1.1:8.4_0": "Urago jiṇṇamivattacaṁ purāṇaṁ.",
  "snp1.1:9.1_0": "Yo nāccasārī na paccasārī,",
  "snp1.1:9.2_0": "Sabbaṁ vitathamidanti ñatva loke;",
  "snp1.1:9.3_0": "So bhikkhu jahāti orapāraṁ,",
  "snp1.1:9.4_0": "Urago jiṇṇamivattacaṁ purāṇaṁ.",
  "snp1.1:10.1_0": "Yo nāccasārī na paccasārī,",
  "snp1.1:10.2_0": "Sabbaṁ vitathamidanti vītalobho;",
  "snp1.1:10.3_0": "So bhikkhu jahāti orapāraṁ,",
  "snp1.1:10.4_0": "Urago jiṇṇamivattacaṁ purāṇaṁ.",
  "snp1.1:11.1_0": "Yo nāccasārī na paccasārī,",
  "snp1.1:11.2_0": "Sabbaṁ vitathamidanti vītarāgo;",
  "snp1.1:11.3_0": "So bhikkhu jahāti orapāraṁ,",
  "snp1.1:11.4_0": "Urago jiṇṇamivattacaṁ purāṇaṁ.",
  "snp1.1:12.1_0": "Yo nāccasārī na paccasārī,",
  "snp1.1:12.2_0": "Sabbaṁ vitathamidanti vītadoso;",
  "snp1.1:12.3_0": "So bhikkhu jahāti orapāraṁ,",
  "snp1.1:12.4_0": "Urago jiṇṇamivattacaṁ purāṇaṁ.",
  "snp1.1:13.1_0": "Yo nāccasārī na paccasārī,",
  "snp1.1:13.2_0": "Sabbaṁ vitathamidanti vītamoho;",
  "snp1.1:13.3_0": "So bhikkhu jahāti orapāraṁ,",
  "snp1.1:13.4_0": "Urago jiṇṇamivattacaṁ purāṇaṁ.",
  "snp1.1:14.1_0": "Yassānusayā na santi keci,",
  "snp1.1:14.2_0": "Mūlā ca akusalā samūhatāse;",
  "snp1.1:14.3_0": "So bhikkhu jahāti orapāraṁ,",
  "snp1.1:14.4_0": "Urago jiṇṇamivattacaṁ purāṇaṁ.",
  "snp1.1:15.1_0": "Yassa darathajā na santi keci,",
  "snp1.1:15.2_0": "Oraṁ āgamanāya paccayāse;",
  "snp1.1:15.3_0": "So bhikkhu jahāti orapāraṁ,",
  "snp1.1:15.4_0": "Urago jiṇṇamivattacaṁ purāṇaṁ.",
  "snp1.1:16.1_0": "Yassa vanathajā na santi keci,",
  "snp1.1:16.2_0": "Vinibandhāya bhavāya hetukappā;",
  "snp1.1:16.3_0": "So bhikkhu jahāti orapāraṁ,",
  "snp1.1:16.4_0": "Urago jiṇṇamivattacaṁ purāṇaṁ.",
  "snp1.1:17.1_0": "Yo nīvaraṇe pahāya pañca,",
  "snp1.1:17.2_0": "Anigho tiṇṇakathaṅkatho visallo;",
  "snp1.1:17.3_0": "So bhikkhu jahāti orapāraṁ,",
  "snp1.1:17.4_0": "Urago jiṇṇamivattacaṁ purāṇanti.",
  "snp1.1:17.5_0": "Uragasuttaṁ paṭhamaṁ."
}