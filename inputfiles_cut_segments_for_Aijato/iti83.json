{
  "iti83:0.1_0": "Itivuttaka 83",
  "iti83:0.2_0": "Tikanipāta",
  "iti83:0.3_0": "Catutthavagga",
  "iti83:0.4_0": "Pañcapubbanimittasutta",
  "iti83:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti83:2.1_0": "“Yadā, bhikkhave, devo devakāyā cavanadhammo hoti, pañcassa pubbanimittāni pātubhavanti—",
  "iti83:2.2_0": "mālā milāyanti, vatthāni kilissanti, kacchehi sedā muccanti, kāye dubbaṇṇiyaṁ okkamati, sake devo devāsane nābhiramatīti.",
  "iti83:2.3_0": "Tamenaṁ, bhikkhave, devā ‘cavanadhammo ayaṁ devaputto’ti iti viditvā tīhi vācāhi anumodenti:",
  "iti83:2.4_0": "‘ito, bho, sugatiṁ gaccha.",
  "iti83:3.1_0": "Sugatiṁ gantvā suladdhalābhaṁ labha.",
  "iti83:4.1_0": "Suladdhalābhaṁ labhitvā suppatiṭṭhito bhavāhī’”ti.",
  "iti83:5.1_0": "Evaṁ vutte, aññataro bhikkhu bhagavantaṁ etadavoca:",
  "iti83:5.2_0": "“kiṁ nu kho, bhante, devānaṁ sugatigamanasaṅkhātaṁ.",
  "iti83:6.1_0": "Kiñca, bhante, devānaṁ suladdhalābhasaṅkhātaṁ.",
  "iti83:7.1_0": "Kiṁ pana, bhante, devānaṁ suppatiṭṭhitasaṅkhātan”ti?",
  "iti83:8.1_0": "“Manussattaṁ kho, bhikkhu, devānaṁ sugatigamanasaṅkhātaṁ.",
  "iti83:9.1_0": "Yaṁ manussabhūto samāno tathāgatappavedite dhammavinaye saddhaṁ paṭilabhati;",
  "iti83:9.2_0": "idaṁ kho, bhikkhu, devānaṁ suladdhalābhasaṅkhātaṁ.",
  "iti83:10.1_0": "Sā kho panassa saddhā niviṭṭhā hoti mūlajātā patiṭṭhitā daḷhā asaṁhāriyā samaṇena vā brāhmaṇena vā devena vā mārena vā brahmunā vā kenaci vā lokasmiṁ;",
  "iti83:10.2_0": "idaṁ kho, bhikkhu, devānaṁ suppatiṭṭhitasaṅkhātan”ti.",
  "iti83:11.1_0": "Etamatthaṁ bhagavā avoca.",
  "iti83:11.2_0": "Tatthetaṁ iti vuccati:",
  "iti83:12.1_0": "“Yadā devo devakāyā,",
  "iti83:12.2_0": "cavati āyusaṅkhayā;",
  "iti83:12.3_0": "Tayo saddā niccharanti,",
  "iti83:12.4_0": "devānaṁ anumodataṁ.",
  "iti83:13.1_0": "‘Ito bho sugatiṁ gaccha,",
  "iti83:13.2_0": "manussānaṁ sahabyataṁ;",
  "iti83:13.3_0": "Manussabhūto saddhamme,",
  "iti83:13.4_0": "labha saddhaṁ anuttaraṁ.",
  "iti83:14.1_0": "Sā te saddhā niviṭṭhassa,",
  "iti83:14.2_0": "mūlajātā patiṭṭhitā;",
  "iti83:14.3_0": "Yāvajīvaṁ asaṁhīrā,",
  "iti83:14.4_0": "saddhamme suppavedite.",
  "iti83:15.1_0": "Kāyaduccaritaṁ hitvā,",
  "iti83:15.2_0": "vacīduccaritāni ca;",
  "iti83:15.3_0": "Manoduccaritaṁ hitvā,",
  "iti83:15.4_0": "yañcaññaṁ dosasañhitaṁ.",
  "iti83:16.1_0": "Kāyena kusalaṁ katvā,",
  "iti83:16.2_0": "vācāya kusalaṁ bahuṁ;",
  "iti83:16.3_0": "Manasā kusalaṁ katvā,",
  "iti83:16.4_0": "appamāṇaṁ nirūpadhiṁ.",
  "iti83:17.1_0": "Tato opadhikaṁ puññaṁ,",
  "iti83:17.2_0": "katvā dānena taṁ bahuṁ;",
  "iti83:17.3_0": "Aññepi macce saddhamme,",
  "iti83:17.4_0": "brahmacariye nivesaya’.",
  "iti83:18.1_0": "Imāya anukampāya,",
  "iti83:18.2_0": "devā devaṁ yadā vidū;",
  "iti83:18.3_0": "Cavantaṁ anumodenti,",
  "iti83:18.4_0": "ehi deva punappunan”ti.",
  "iti83:19.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti83:19.2_0": "Catutthaṁ."
}