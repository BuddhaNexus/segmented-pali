{
  "tha-ap263:0.1_0": "Therāpadāna",
  "tha-ap263:0.2_0": "Padumukkhipavagga",
  "tha-ap263:0.3_0": "1. Ākāsukkhipiyattheraapadāna",
  "tha-ap263:1.1_0": "“Suvaṇṇavaṇṇaṁ siddhatthaṁ,",
  "tha-ap263:1.2_0": "Gacchantaṁ antarāpaṇe;",
  "tha-ap263:1.3_0": "Jalajagge duve gayha,",
  "tha-ap263:1.4_0": "Upāgacchiṁ narāsabhaṁ.",
  "tha-ap263:2.1_0": "Ekañca pupphaṁ pādesu,",
  "tha-ap263:2.2_0": "buddhaseṭṭhassa nikkhipiṁ;",
  "tha-ap263:2.3_0": "Ekañca pupphaṁ paggayha,",
  "tha-ap263:2.4_0": "ākāse ukkhipiṁ ahaṁ.",
  "tha-ap263:3.1_0": "Catunnavutito kappe,",
  "tha-ap263:3.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap263:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap263:3.4_0": "pupphadānassidaṁ phalaṁ.",
  "tha-ap263:4.1_0": "Ito chattiṁsakappamhi,",
  "tha-ap263:4.2_0": "eko āsiṁ mahīpati;",
  "tha-ap263:4.3_0": "Antalikkhakaro nāma,",
  "tha-ap263:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap263:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap263:5.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap263:5.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap263:5.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap263:6.1_0": "Itthaṁ sudaṁ āyasmā ākāsukkhipiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap263:7.1_0": "Ākāsukkhipiyattherassāpadānaṁ paṭhamaṁ."
}