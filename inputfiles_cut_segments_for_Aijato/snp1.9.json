{
  "snp1.9:0.1_0": "Sutta Nipāta 1.9",
  "snp1.9:0.2_0": "Hemavatasutta",
  "snp1.9:1.1_0": "“Ajja pannaraso uposatho,",
  "snp1.9:1.2_0": "(iti sātāgiro yakkho)",
  "snp1.9:1.3_0": "Dibbā ratti upaṭṭhitā;",
  "snp1.9:1.4_0": "Anomanāmaṁ satthāraṁ,",
  "snp1.9:1.5_0": "Handa passāma gotamaṁ”.",
  "snp1.9:2.1_0": "“Kacci mano supaṇihito,",
  "snp1.9:2.2_0": "(iti hemavato yakkho)",
  "snp1.9:2.3_0": "Sabbabhūtesu tādino;",
  "snp1.9:2.4_0": "Kacci iṭṭhe aniṭṭhe ca,",
  "snp1.9:2.5_0": "Saṅkappassa vasīkatā”.",
  "snp1.9:3.1_0": "“Mano cassa supaṇihito,",
  "snp1.9:3.2_0": "(iti sātāgiro yakkho)",
  "snp1.9:3.3_0": "Sabbabhūtesu tādino;",
  "snp1.9:3.4_0": "Atho iṭṭhe aniṭṭhe ca,",
  "snp1.9:3.5_0": "Saṅkappassa vasīkatā”.",
  "snp1.9:4.1_0": "“Kacci adinnaṁ nādiyati,",
  "snp1.9:4.2_0": "(iti hemavato yakkho)",
  "snp1.9:4.3_0": "Kacci pāṇesu saññato;",
  "snp1.9:4.4_0": "Kacci ārā pamādamhā,",
  "snp1.9:4.5_0": "Kacci jhānaṁ na riñcati”.",
  "snp1.9:5.1_0": "“Na so adinnaṁ ādiyati,",
  "snp1.9:5.2_0": "(iti sātāgiro yakkho)",
  "snp1.9:5.3_0": "Atho pāṇesu saññato;",
  "snp1.9:5.4_0": "Atho ārā pamādamhā,",
  "snp1.9:5.5_0": "Buddho jhānaṁ na riñcati”.",
  "snp1.9:6.1_0": "“Kacci musā na bhaṇati,",
  "snp1.9:6.2_0": "(iti hemavato yakkho)",
  "snp1.9:6.3_0": "Kacci na khīṇabyappatho;",
  "snp1.9:6.4_0": "Kacci vebhūtiyaṁ nāha,",
  "snp1.9:6.5_0": "Kacci samphaṁ na bhāsati”.",
  "snp1.9:7.1_0": "“Musā ca so na bhaṇati,",
  "snp1.9:7.2_0": "(iti sātāgiro yakkho)",
  "snp1.9:7.3_0": "Atho na khīṇabyappatho;",
  "snp1.9:7.4_0": "Atho vebhūtiyaṁ nāha,",
  "snp1.9:7.5_0": "Mantā atthañca bhāsati”.",
  "snp1.9:8.1_0": "“Kacci na rajjati kāmesu,",
  "snp1.9:8.2_0": "(iti hemavato yakkho)",
  "snp1.9:8.3_0": "Kacci cittaṁ anāvilaṁ;",
  "snp1.9:8.4_0": "Kacci mohaṁ atikkanto,",
  "snp1.9:8.5_0": "Kacci dhammesu cakkhumā”.",
  "snp1.9:9.1_0": "“Na so rajjati kāmesu,",
  "snp1.9:9.2_0": "(iti sātāgiro yakkho)",
  "snp1.9:9.3_0": "Atho cittaṁ anāvilaṁ;",
  "snp1.9:9.4_0": "Sabbamohaṁ atikkanto,",
  "snp1.9:9.5_0": "Buddho dhammesu cakkhumā”.",
  "snp1.9:10.1_0": "“Kacci vijjāya sampanno,",
  "snp1.9:10.2_0": "(iti hemavato yakkho)",
  "snp1.9:10.3_0": "Kacci saṁsuddhacāraṇo;",
  "snp1.9:10.4_0": "Kaccissa āsavā khīṇā,",
  "snp1.9:10.5_0": "Kacci natthi punabbhavo”.",
  "snp1.9:11.1_0": "“Vijjāya ceva sampanno,",
  "snp1.9:11.2_0": "(iti sātāgiro yakkho)",
  "snp1.9:11.3_0": "Atho saṁsuddhacāraṇo;",
  "snp1.9:11.4_0": "Sabbassa āsavā khīṇā,",
  "snp1.9:11.5_0": "Natthi tassa punabbhavo”.",
  "snp1.9:12.1_0": "“Sampannaṁ munino cittaṁ,",
  "snp1.9:12.2_0": "Kammunā byappathena ca;",
  "snp1.9:12.3_0": "Vijjācaraṇasampannaṁ,",
  "snp1.9:12.4_0": "Dhammato naṁ pasaṁsati”.",
  "snp1.9:13.1_0": "“Sampannaṁ munino cittaṁ,",
  "snp1.9:13.2_0": "Kammunā byappathena ca;",
  "snp1.9:13.3_0": "Vijjācaraṇasampannaṁ,",
  "snp1.9:13.4_0": "Dhammato anumodasi.",
  "snp1.9:14.1_0": "Sampannaṁ munino cittaṁ,",
  "snp1.9:14.2_0": "Kammunā byappathena ca;",
  "snp1.9:14.3_0": "Vijjācaraṇasampannaṁ,",
  "snp1.9:14.4_0": "Handa passāma gotamaṁ”.",
  "snp1.9:15.1_0": "“Eṇijaṅghaṁ kisaṁ vīraṁ,",
  "snp1.9:15.2_0": "Appāhāraṁ alolupaṁ;",
  "snp1.9:15.3_0": "Muniṁ vanasmiṁ jhāyantaṁ,",
  "snp1.9:15.4_0": "Ehi passāma gotamaṁ.",
  "snp1.9:16.1_0": "Sīhaṁvekacaraṁ nāgaṁ,",
  "snp1.9:16.2_0": "Kāmesu anapekkhinaṁ;",
  "snp1.9:16.3_0": "Upasaṅkamma pucchāma,",
  "snp1.9:16.4_0": "Maccupāsappamocanaṁ.",
  "snp1.9:17.1_0": "Akkhātāraṁ pavattāraṁ,",
  "snp1.9:17.2_0": "Sabbadhammāna pāraguṁ;",
  "snp1.9:17.3_0": "Buddhaṁ verabhayātītaṁ,",
  "snp1.9:17.4_0": "Mayaṁ pucchāma gotamaṁ”.",
  "snp1.9:18.1_0": "“Kismiṁ loko samuppanno,",
  "snp1.9:18.2_0": "(iti hemavato yakkho)",
  "snp1.9:18.3_0": "Kismiṁ kubbati santhavaṁ;",
  "snp1.9:18.4_0": "Kissa loko upādāya,",
  "snp1.9:18.5_0": "Kismiṁ loko vihaññati”.",
  "snp1.9:19.1_0": "“Chasu loko samuppanno,",
  "snp1.9:19.2_0": "(hemavatāti bhagavā)",
  "snp1.9:19.3_0": "Chasu kubbati santhavaṁ;",
  "snp1.9:19.4_0": "Channameva upādāya,",
  "snp1.9:19.5_0": "Chasu loko vihaññati”.",
  "snp1.9:20.1_0": "“Katamaṁ taṁ upādānaṁ,",
  "snp1.9:20.2_0": "Yattha loko vihaññati;",
  "snp1.9:20.3_0": "Niyyānaṁ pucchito brūhi,",
  "snp1.9:20.4_0": "Kathaṁ dukkhā pamuccati”.",
  "snp1.9:21.1_0": "“Pañca kāmaguṇā loke,",
  "snp1.9:21.2_0": "Manochaṭṭhā paveditā;",
  "snp1.9:21.3_0": "Ettha chandaṁ virājetvā,",
  "snp1.9:21.4_0": "Evaṁ dukkhā pamuccati.",
  "snp1.9:22.1_0": "Etaṁ lokassa niyyānaṁ,",
  "snp1.9:22.2_0": "Akkhātaṁ vo yathātathaṁ;",
  "snp1.9:22.3_0": "Etaṁ vo ahamakkhāmi,",
  "snp1.9:22.4_0": "Evaṁ dukkhā pamuccati”.",
  "snp1.9:23.1_0": "“Ko sūdha tarati oghaṁ,",
  "snp1.9:23.2_0": "Kodha tarati aṇṇavaṁ;",
  "snp1.9:23.3_0": "Appatiṭṭhe anālambe,",
  "snp1.9:23.4_0": "Ko gambhīre na sīdati”.",
  "snp1.9:24.1_0": "“Sabbadā sīlasampanno,",
  "snp1.9:24.2_0": "Paññavā susamāhito;",
  "snp1.9:24.3_0": "Ajjhattacintī satimā,",
  "snp1.9:24.4_0": "Oghaṁ tarati duttaraṁ.",
  "snp1.9:25.1_0": "Virato kāmasaññāya,",
  "snp1.9:25.2_0": "Sabbasaṁyojanātigo;",
  "snp1.9:25.3_0": "Nandībhavaparikkhīṇo,",
  "snp1.9:25.4_0": "So gambhīre na sīdati”.",
  "snp1.9:26.1_0": "“Gambhīrapaññaṁ nipuṇatthadassiṁ,",
  "snp1.9:26.2_0": "Akiñcanaṁ kāmabhave asattaṁ;",
  "snp1.9:26.3_0": "Taṁ passatha sabbadhi vippamuttaṁ,",
  "snp1.9:26.4_0": "Dibbe pathe kamamānaṁ mahesiṁ.",
  "snp1.9:27.1_0": "Anomanāmaṁ nipuṇatthadassiṁ,",
  "snp1.9:27.2_0": "Paññādadaṁ kāmālaye asattaṁ;",
  "snp1.9:27.3_0": "Taṁ passatha sabbaviduṁ sumedhaṁ,",
  "snp1.9:27.4_0": "Ariye pathe kamamānaṁ mahesiṁ.",
  "snp1.9:28.1_0": "Sudiṭṭhaṁ vata no ajja,",
  "snp1.9:28.2_0": "Suppabhātaṁ suhuṭṭhitaṁ;",
  "snp1.9:28.3_0": "Yaṁ addasāma sambuddhaṁ,",
  "snp1.9:28.4_0": "Oghatiṇṇamanāsavaṁ.",
  "snp1.9:29.1_0": "Ime dasasatā yakkhā,",
  "snp1.9:29.2_0": "Iddhimanto yasassino;",
  "snp1.9:29.3_0": "Sabbe taṁ saraṇaṁ yanti,",
  "snp1.9:29.4_0": "Tvaṁ no satthā anuttaro.",
  "snp1.9:30.1_0": "Te mayaṁ vicarissāma,",
  "snp1.9:30.2_0": "Gāmā gāmaṁ nagā nagaṁ;",
  "snp1.9:30.3_0": "Namassamānā sambuddhaṁ,",
  "snp1.9:30.4_0": "Dhammassa ca sudhammatan”ti.",
  "snp1.9:30.5_0": "Hemavatasuttaṁ navamaṁ."
}