{
  "tha-ap267:0.1_0": "Therāpadāna",
  "tha-ap267:0.2_0": "Padumukkhipavagga",
  "tha-ap267:0.3_0": "5. Biḷālidāyakattheraapadāna",
  "tha-ap267:1.1_0": "“Himavantassāvidūre,",
  "tha-ap267:1.2_0": "romaso nāma pabbato;",
  "tha-ap267:1.3_0": "Tamhi pabbatapādamhi,",
  "tha-ap267:1.4_0": "samaṇo bhāvitindriyo.",
  "tha-ap267:2.1_0": "Biḷāliyo gahetvāna,",
  "tha-ap267:2.2_0": "samaṇassa adāsahaṁ;",
  "tha-ap267:2.3_0": "Anumodi mahāvīro,",
  "tha-ap267:2.4_0": "sayambhū aparājito.",
  "tha-ap267:3.1_0": "Biḷālī te mama dinnā,",
  "tha-ap267:3.2_0": "vippasannena cetasā;",
  "tha-ap267:3.3_0": "Bhave nibbattamānamhi,",
  "tha-ap267:3.4_0": "phalaṁ nibbattataṁ tava.",
  "tha-ap267:4.1_0": "Catunnavutito kappe,",
  "tha-ap267:4.2_0": "yaṁ biḷālimadāsahaṁ;",
  "tha-ap267:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap267:4.4_0": "biḷāliyā idaṁ phalaṁ.",
  "tha-ap267:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap267:5.2_0": "…pe…",
  "tha-ap267:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap267:6.1_0": "Itthaṁ sudaṁ āyasmā biḷālidāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap267:7.1_0": "Biḷālidāyakattherassāpadānaṁ pañcamaṁ."
}