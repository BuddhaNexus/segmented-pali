{
  "ja411:0.1_0": "Jātaka",
  "ja411:0.2_0": "Sattakanipāta",
  "ja411:0.3_0": "Gandhāravagga",
  "ja411:0.4_0": "6. Susīmajātaka",
  "ja411:1.1_0": "“Kāḷāni kesāni pure ahesuṁ,",
  "ja411:1.2_0": "Jātāni sīsamhi yathāpadese;",
  "ja411:1.3_0": "Tānajja setāni susīma disvā,",
  "ja411:1.4_0": "Dhammaṁ cara brahmacariyassa kālo”.",
  "ja411:2.1_0": "“Mameva deva palitaṁ na tuyhaṁ,",
  "ja411:2.2_0": "Mameva sīsaṁ mama uttamaṅgaṁ;",
  "ja411:2.3_0": "‘Atthaṁ karissan’ti musā abhāṇiṁ,",
  "ja411:2.4_0": "Ekāparādhaṁ khama rājaseṭṭha.",
  "ja411:3.1_0": "Daharo tuvaṁ dassaniyosi rāja,",
  "ja411:3.2_0": "Paṭhamuggato hosi yathā kaḷīro;",
  "ja411:3.3_0": "Rajjañca kārehi mamañca passa,",
  "ja411:3.4_0": "Mā kālikaṁ anudhāvī janinda”.",
  "ja411:4.1_0": "“Passāmi vohaṁ dahariṁ kumāriṁ,",
  "ja411:4.2_0": "Sāmaṭṭhapassaṁ sutanuṁ sumajjhaṁ;",
  "ja411:4.3_0": "Kāḷappavāḷāva pavellamānā,",
  "ja411:4.4_0": "Palobhayantīva naresu gacchati.",
  "ja411:5.1_0": "Tamena passāmiparena nāriṁ,",
  "ja411:5.2_0": "Āsītikaṁ nāvutikaṁ va jaccā;",
  "ja411:5.3_0": "Daṇḍaṁ gahetvāna pavedhamānaṁ,",
  "ja411:5.4_0": "Gopānasībhoggasamaṁ carantiṁ.",
  "ja411:6.1_0": "Sohaṁ tamevānuvicintayanto,",
  "ja411:6.2_0": "Eko sayāmi sayanassa majjhe;",
  "ja411:6.3_0": "‘Ahampi evaṁ’ iti pekkhamāno,",
  "ja411:6.4_0": "Na gahe rame brahmacariyassa kālo.",
  "ja411:7.1_0": "Rajjuvālambanī cesā,",
  "ja411:7.2_0": "Yā gehe vasato rati;",
  "ja411:7.3_0": "Evampi chetvāna vajanti dhīrā,",
  "ja411:7.4_0": "Anapekkhino kāmasukhaṁ pahāyā”ti.",
  "ja411:8.1_0": "Susīmajātakaṁ chaṭṭhaṁ."
}