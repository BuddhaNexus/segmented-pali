{
  "tha-ap380:0.1_0": "Therāpadāna",
  "tha-ap380:0.2_0": "Bodhivandanavagga",
  "tha-ap380:0.3_0": "8. Sumanatālavaṇṭiyattheraapadāna",
  "tha-ap380:1.1_0": "“Siddhatthassa bhagavato,",
  "tha-ap380:1.2_0": "tālavaṇṭamadāsahaṁ;",
  "tha-ap380:1.3_0": "Sumanehi paṭicchannaṁ,",
  "tha-ap380:1.4_0": "dhārayāmi mahārahaṁ.",
  "tha-ap380:2.1_0": "Catunnavutito kappe,",
  "tha-ap380:2.2_0": "tālavaṇṭamadāsahaṁ;",
  "tha-ap380:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap380:2.4_0": "tālavaṇṭassidaṁ phalaṁ.",
  "tha-ap380:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap380:3.2_0": "…pe…",
  "tha-ap380:3.3_0": "viharāmi anāsavo.",
  "tha-ap380:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap380:4.2_0": "…pe…",
  "tha-ap380:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap380:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap380:5.2_0": "…pe…",
  "tha-ap380:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap380:6.1_0": "Itthaṁ sudaṁ āyasmā sumanatālavaṇṭiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap380:7.1_0": "Sumanatālavaṇṭiyattherassāpadānaṁ aṭṭhamaṁ."
}