{
  "thag12.2:0.1_0": "Theragāthā",
  "thag12.2:0.2_0": "Dvādasakanipāta",
  "thag12.2:0.3_0": "Paṭhamavagga",
  "thag12.2:0.4_0": "12.2. Sunītattheragāthā",
  "thag12.2:1.1_0": "“Nīce kulamhi jātohaṁ,",
  "thag12.2:1.2_0": "daliddo appabhojano;",
  "thag12.2:1.3_0": "Hīnakammaṁ mamaṁ āsi,",
  "thag12.2:1.4_0": "ahosiṁ pupphachaḍḍako.",
  "thag12.2:2.1_0": "Jigucchito manussānaṁ,",
  "thag12.2:2.2_0": "paribhūto ca vambhito;",
  "thag12.2:2.3_0": "Nīcaṁ manaṁ karitvāna,",
  "thag12.2:2.4_0": "vandissaṁ bahukaṁ janaṁ.",
  "thag12.2:3.1_0": "Athaddasāsiṁ sambuddhaṁ,",
  "thag12.2:3.2_0": "bhikkhusaṅghapurakkhataṁ;",
  "thag12.2:3.3_0": "Pavisantaṁ mahāvīraṁ,",
  "thag12.2:3.4_0": "magadhānaṁ puruttamaṁ.",
  "thag12.2:4.1_0": "Nikkhipitvāna byābhaṅgiṁ,",
  "thag12.2:4.2_0": "vandituṁ upasaṅkamiṁ;",
  "thag12.2:4.3_0": "Mameva anukampāya,",
  "thag12.2:4.4_0": "aṭṭhāsi purisuttamo.",
  "thag12.2:5.1_0": "Vanditvā satthuno pāde,",
  "thag12.2:5.2_0": "ekamantaṁ ṭhito tadā;",
  "thag12.2:5.3_0": "Pabbajjaṁ ahamāyāciṁ,",
  "thag12.2:5.4_0": "sabbasattānamuttamaṁ.",
  "thag12.2:6.1_0": "Tato kāruṇiko satthā,",
  "thag12.2:6.2_0": "sabbalokānukampako;",
  "thag12.2:6.3_0": "‘Ehi bhikkhū’ti maṁ āha,",
  "thag12.2:6.4_0": "sā me āsūpasampadā.",
  "thag12.2:7.1_0": "Sohaṁ eko araññasmiṁ,",
  "thag12.2:7.2_0": "viharanto atandito;",
  "thag12.2:7.3_0": "Akāsiṁ satthu vacanaṁ,",
  "thag12.2:7.4_0": "yathā maṁ ovadī jino.",
  "thag12.2:8.1_0": "Rattiyā paṭhamaṁ yāmaṁ,",
  "thag12.2:8.2_0": "Pubbajātimanussariṁ;",
  "thag12.2:8.3_0": "Rattiyā majjhimaṁ yāmaṁ,",
  "thag12.2:8.4_0": "Dibbacakkhuṁ visodhayiṁ;",
  "thag12.2:8.5_0": "Rattiyā pacchime yāme,",
  "thag12.2:8.6_0": "Tamokhandhaṁ padālayiṁ.",
  "thag12.2:9.1_0": "Tato ratyā vivasāne,",
  "thag12.2:9.2_0": "sūriyassuggamanaṁ pati;",
  "thag12.2:9.3_0": "Indo brahmā ca āgantvā,",
  "thag12.2:9.4_0": "maṁ namassiṁsu pañjalī.",
  "thag12.2:10.1_0": "‘Namo te purisājañña,",
  "thag12.2:10.2_0": "namo te purisuttama;",
  "thag12.2:10.3_0": "Yassa te āsavā khīṇā,",
  "thag12.2:10.4_0": "dakkhiṇeyyosi mārisa’.",
  "thag12.2:11.1_0": "Tato disvāna maṁ satthā,",
  "thag12.2:11.2_0": "devasaṅghapurakkhataṁ;",
  "thag12.2:11.3_0": "Sitaṁ pātukaritvāna,",
  "thag12.2:11.4_0": "imamatthaṁ abhāsatha.",
  "thag12.2:12.1_0": "‘Tapena brahmacariyena,",
  "thag12.2:12.2_0": "saṁyamena damena ca;",
  "thag12.2:12.3_0": "Etena brāhmaṇo hoti,",
  "thag12.2:12.4_0": "etaṁ brāhmaṇamuttaman’”ti.",
  "thag12.2:12.5_0": "…",
  "thag12.2:12.6_0": "Sunīto thero ….",
  "thag12.2:12.7_0": "Dvādasakanipāto niṭṭhito.",
  "thag12.2:13.0_0": "Tatruddānaṁ",
  "thag12.2:13.1_0": "Sīlavā ca sunīto ca,",
  "thag12.2:13.2_0": "therā dve te mahiddhikā;",
  "thag12.2:13.3_0": "Dvādasamhi nipātamhi,",
  "thag12.2:13.4_0": "gāthāyo catuvīsatīti."
}