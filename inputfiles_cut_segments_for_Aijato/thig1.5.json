{
  "thig1.5:0.1_0": "Therīgāthā",
  "thig1.5:0.2_0": "Ekakanipāta",
  "thig1.5:0.3_0": "5. Aññatarātissātherīgāthā",
  "thig1.5:1.1_0": "“Tisse yuñjassu dhammehi,",
  "thig1.5:1.2_0": "khaṇo taṁ mā upaccagā;",
  "thig1.5:1.3_0": "Khaṇātītā hi socanti,",
  "thig1.5:1.4_0": "nirayamhi samappitā”ti.",
  "thig1.5:2.1_0": "… Aññatarā tissā therī …."
}