{
  "ja20:0.1_0": "Jātaka",
  "ja20:0.2_0": "Ekakanipāta",
  "ja20:0.3_0": "Sīlavagga",
  "ja20:0.4_0": "10. Naḷapānajātaka",
  "ja20:1.1_0": "“Disvā padamanuttiṇṇaṁ,",
  "ja20:1.2_0": "Disvānotaritaṁ padaṁ;",
  "ja20:1.3_0": "Naḷena vāriṁ pissāma,",
  "ja20:1.4_0": "Neva maṁ tvaṁ vadhissasī”ti.",
  "ja20:2.1_0": "Naḷapānajātakaṁ dasamaṁ.",
  "ja20:2.2_0": "Sīlavaggo dutiyo.",
  "ja20:3.1_0": "Tassuddānaṁ",
  "ja20:4.1_0": "Atha lakkhaṇa sākha dhiratthu puna,",
  "ja20:4.2_0": "Na kiratthi rasehi kharādiyā;",
  "ja20:4.3_0": "Atibhoti vara māluta pāṇa,",
  "ja20:4.4_0": "Muccena naḷaavhayanena bhavanti dasāti."
}