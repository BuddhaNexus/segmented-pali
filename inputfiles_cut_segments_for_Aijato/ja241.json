{
  "ja241:0.1_0": "Jātaka",
  "ja241:0.2_0": "Dukanipāta",
  "ja241:0.3_0": "Siṅgālavagga",
  "ja241:0.4_0": "1. Sabbadāṭhijātaka",
  "ja241:1.1_0": "“Siṅgālo mānathaddho ca,",
  "ja241:1.2_0": "parivārena atthiko;",
  "ja241:1.3_0": "Pāpuṇi mahatiṁ bhūmiṁ,",
  "ja241:1.4_0": "rājāsi sabbadāṭhinaṁ.",
  "ja241:2.1_0": "Evameva manussesu,",
  "ja241:2.2_0": "yo hoti parivāravā;",
  "ja241:2.3_0": "So hi tattha mahā hoti,",
  "ja241:2.4_0": "siṅgālo viya dāṭhinan”ti.",
  "ja241:3.1_0": "Sabbadāṭhijātakaṁ paṭhamaṁ."
}