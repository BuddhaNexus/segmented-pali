{
  "kv4.4:0.1_0": "Kathāvatthu",
  "kv4.4:0.2_0": "Mahāpaṇṇāsaka",
  "kv4.4:0.3_0": "Catutthavagga",
  "kv4.4:0.4_0": "Samannāgatakathā",
  "kv4.4:1.1_0": "Arahā catūhi phalehi samannāgatoti?",
  "kv4.4:1.2_0": "Āmantā.",
  "kv4.4:1.3_0": "Arahā catūhi phassehi catūhi vedanāhi catūhi saññāhi catūhi cetanāhi catūhi cittehi catūhi saddhāhi catūhi vīriyehi catūhi satīhi catūhi samādhīhi catūhi paññāhi samannāgatoti?",
  "kv4.4:1.4_0": "Na hevaṁ vattabbe …pe….",
  "kv4.4:2.1_0": "Anāgāmī tīhi phalehi samannāgatoti?",
  "kv4.4:2.2_0": "Āmantā.",
  "kv4.4:2.3_0": "Anāgāmī tīhi phassehi …pe… tīhi paññāhi samannāgatoti?",
  "kv4.4:2.4_0": "Na hevaṁ vattabbe …pe….",
  "kv4.4:3.1_0": "Sakadāgāmī dvīhi phalehi samannāgatoti?",
  "kv4.4:3.2_0": "Āmantā.",
  "kv4.4:3.3_0": "Sakadāgāmī dvīhi phassehi …pe… dvīhi paññāhi samannāgatoti?",
  "kv4.4:3.4_0": "Na hevaṁ vattabbe …pe….",
  "kv4.4:4.1_0": "Arahā sotāpattiphalena samannāgatoti?",
  "kv4.4:4.2_0": "Āmantā.",
  "kv4.4:4.3_0": "Arahā sotāpanno sattakkhattuparamo, kolaṅkolo, ekabījīti?",
  "kv4.4:4.4_0": "Na hevaṁ vattabbe …pe…",
  "kv4.4:4.5_0": "arahā sakadāgāmiphalena samannāgatoti?",
  "kv4.4:4.6_0": "Āmantā.",
  "kv4.4:4.7_0": "Arahā sakadāgāmīti?",
  "kv4.4:4.8_0": "Na hevaṁ vattabbe …pe…",
  "kv4.4:4.9_0": "arahā anāgāmiphalena samannāgatoti?",
  "kv4.4:4.10_0": "Āmantā.",
  "kv4.4:4.11_0": "Arahā anāgāmī, antarāparinibbāyī, upahaccaparinibbāyī, asaṅkhāraparinibbāyī, sasaṅkhāraparinibbāyī, uddhaṁsoto akaniṭṭhagāmīti?",
  "kv4.4:4.12_0": "Na hevaṁ vattabbe …pe….",
  "kv4.4:5.1_0": "Anāgāmī sotāpattiphalena samannāgatoti?",
  "kv4.4:5.2_0": "Āmantā.",
  "kv4.4:5.3_0": "Anāgāmī sotāpanno sattakkhattuparamo, kolaṅkolo, ekabījīti?",
  "kv4.4:5.4_0": "Na hevaṁ vattabbe …pe…",
  "kv4.4:5.5_0": "anāgāmī sakadāgāmiphalena samannāgatoti?",
  "kv4.4:5.6_0": "Āmantā.",
  "kv4.4:5.7_0": "Anāgāmī sakadāgāmīti?",
  "kv4.4:5.8_0": "Na hevaṁ vattabbe …pe….",
  "kv4.4:6.1_0": "Sakadāgāmī sotāpattiphalena samannāgatoti?",
  "kv4.4:6.2_0": "Āmantā.",
  "kv4.4:6.3_0": "Sakadāgāmī sotāpanno sattakkhattuparamo, kolaṅkolo, ekabījīti?",
  "kv4.4:6.4_0": "Na hevaṁ vattabbe …pe….",
  "kv4.4:7.1_0": "Sotāpattiphalena samannāgato “sotāpanno”ti vattabboti?",
  "kv4.4:7.2_0": "Āmantā.",
  "kv4.4:7.3_0": "Arahā sotāpattiphalena samannāgatoti?",
  "kv4.4:7.4_0": "Āmantā.",
  "kv4.4:7.5_0": "Sveva arahā, so sotāpannoti?",
  "kv4.4:7.6_0": "Na hevaṁ vattabbe …pe….",
  "kv4.4:8.1_0": "Sakadāgāmiphalena samannāgato “sakadāgāmī”ti vattabboti?",
  "kv4.4:8.2_0": "Āmantā.",
  "kv4.4:8.3_0": "Arahā sakadāgāmiphalena samannāgatoti?",
  "kv4.4:8.4_0": "Āmantā.",
  "kv4.4:8.5_0": "Sveva arahā, so sakadāgāmīti?",
  "kv4.4:8.6_0": "Na hevaṁ vattabbe …pe….",
  "kv4.4:9.1_0": "Anāgāmiphalena samannāgato “anāgāmī”ti vattabboti?",
  "kv4.4:9.2_0": "Āmantā.",
  "kv4.4:9.3_0": "Arahā anāgāmiphalena samannāgatoti?",
  "kv4.4:9.4_0": "Āmantā.",
  "kv4.4:9.5_0": "Sveva arahā, so anāgāmīti?",
  "kv4.4:9.6_0": "Na hevaṁ vattabbe …pe….",
  "kv4.4:10.1_0": "Sotāpattiphalena samannāgato “sotāpanno”ti vattabboti?",
  "kv4.4:10.2_0": "Āmantā.",
  "kv4.4:10.3_0": "Anāgāmī sotāpattiphalena samannāgatoti?",
  "kv4.4:10.4_0": "Āmantā.",
  "kv4.4:10.5_0": "Sveva anāgāmī, so sotāpannoti?",
  "kv4.4:10.6_0": "Na hevaṁ vattabbe …pe….",
  "kv4.4:11.1_0": "Sakadāgāmiphalena samannāgato “sakadāgāmī”ti vattabboti?",
  "kv4.4:11.2_0": "Āmantā.",
  "kv4.4:11.3_0": "Anāgāmī sakadāgāmiphalena samannāgatoti?",
  "kv4.4:11.4_0": "Āmantā.",
  "kv4.4:11.5_0": "Sveva anāgāmī, so sakadāgāmīti?",
  "kv4.4:11.6_0": "Na hevaṁ vattabbe …pe….",
  "kv4.4:12.1_0": "Sotāpattiphalena samannāgato “sotāpanno”ti vattabboti?",
  "kv4.4:12.2_0": "Āmantā.",
  "kv4.4:12.3_0": "Sakadāgāmī sotāpattiphalena samannāgatoti?",
  "kv4.4:12.4_0": "Āmantā.",
  "kv4.4:12.5_0": "Sveva sakadāgāmī, so sotāpannoti?",
  "kv4.4:12.6_0": "Na hevaṁ vattabbe …pe….",
  "kv4.4:13.1_0": "Arahā sotāpattiphalena samannāgatoti?",
  "kv4.4:13.2_0": "Āmantā.",
  "kv4.4:13.3_0": "Nanu arahā sotāpattiphalaṁ vītivattoti?",
  "kv4.4:13.4_0": "Āmantā.",
  "kv4.4:13.5_0": "Hañci arahā sotāpattiphalaṁ vītivatto, no ca vata re vattabbe—",
  "kv4.4:13.6_0": "“arahā sotāpattiphalena samannāgato”ti.",
  "kv4.4:14.1_0": "Arahā sotāpattiphalaṁ vītivatto tena samannāgatoti?",
  "kv4.4:14.2_0": "Āmantā.",
  "kv4.4:14.3_0": "Arahā sotāpattimaggaṁ vītivatto, sakkāyadiṭṭhiṁ vicikicchaṁ sīlabbataparāmāsaṁ, apāyagamanīyaṁ rāgaṁ, apāyagamanīyaṁ dosaṁ, apāyagamanīyaṁ mohaṁ vītivatto tena samannāgatoti?",
  "kv4.4:14.4_0": "Na hevaṁ vattabbe …pe….",
  "kv4.4:15.1_0": "Arahā sakadāgāmiphalena samannāgatoti?",
  "kv4.4:15.2_0": "Āmantā.",
  "kv4.4:15.3_0": "Nanu arahā sakadāgāmiphalaṁ vītivattoti?",
  "kv4.4:15.4_0": "Āmantā.",
  "kv4.4:15.5_0": "Hañci arahā sakadāgāmiphalaṁ vītivatto, no ca vata re vattabbe—",
  "kv4.4:15.6_0": "“arahā sakadāgāmiphalena samannāgato”ti.",
  "kv4.4:16.1_0": "Arahā sakadāgāmiphalaṁ vītivatto tena samannāgatoti?",
  "kv4.4:16.2_0": "Āmantā.",
  "kv4.4:16.3_0": "Arahā sakadāgāmimaggaṁ vītivatto, oḷārikaṁ kāmarāgaṁ, oḷārikaṁ byāpādaṁ vītivatto tena samannāgatoti?",
  "kv4.4:16.4_0": "Na hevaṁ vattabbe …pe….",
  "kv4.4:17.1_0": "Arahā anāgāmiphalena samannāgatoti?",
  "kv4.4:17.2_0": "Āmantā.",
  "kv4.4:17.3_0": "Nanu arahā anāgāmiphalaṁ vītivattoti?",
  "kv4.4:17.4_0": "Āmantā.",
  "kv4.4:17.5_0": "Hañci arahā anāgāmiphalaṁ vītivatto, no ca vata re vattabbe—",
  "kv4.4:17.6_0": "“arahā anāgāmiphalena samannāgato”ti.",
  "kv4.4:18.1_0": "Arahā anāgāmiphalaṁ vītivatto tena samannāgatoti?",
  "kv4.4:18.2_0": "Āmantā.",
  "kv4.4:18.3_0": "Arahā anāgāmimaggaṁ vītivatto, aṇusahagataṁ kāmarāgaṁ, aṇusahagataṁ byāpādaṁ vītivatto tena samannāgatoti?",
  "kv4.4:18.4_0": "Na hevaṁ vattabbe …pe….",
  "kv4.4:19.1_0": "Anāgāmī sotāpattiphalena samannāgatoti?",
  "kv4.4:19.2_0": "Āmantā.",
  "kv4.4:19.3_0": "Nanu anāgāmī sotāpattiphalaṁ vītivattoti?",
  "kv4.4:19.4_0": "Āmantā.",
  "kv4.4:19.5_0": "Hañci anāgāmī sotāpattiphalaṁ vītivatto, no ca vata re vattabbe—",
  "kv4.4:19.6_0": "“anāgāmī sotāpattiphalena samannāgato”ti.",
  "kv4.4:20.1_0": "Anāgāmī sotāpattiphalaṁ vītivatto tena samannāgatoti?",
  "kv4.4:20.2_0": "Āmantā.",
  "kv4.4:20.3_0": "Anāgāmī sotāpattimaggaṁ vītivatto, sakkāyadiṭṭhiṁ …pe… apāyagamanīyaṁ mohaṁ vītivatto tena samannāgatoti?",
  "kv4.4:20.4_0": "Na hevaṁ vattabbe …pe….",
  "kv4.4:21.1_0": "Anāgāmī sakadāgāmiphalena samannāgatoti?",
  "kv4.4:21.2_0": "Āmantā.",
  "kv4.4:21.3_0": "Nanu anāgāmī sakadāgāmiphalaṁ vītivattoti?",
  "kv4.4:21.4_0": "Āmantā.",
  "kv4.4:21.5_0": "Hañci anāgāmī sakadāgāmiphalaṁ vītivatto, no ca vata re vattabbe—",
  "kv4.4:21.6_0": "“anāgāmī sakadāgāmiphalena samannāgato”ti.",
  "kv4.4:22.1_0": "Anāgāmī sakadāgāmiphalaṁ vītivatto tena samannāgatoti?",
  "kv4.4:22.2_0": "Āmantā.",
  "kv4.4:22.3_0": "Anāgāmī sakadāgāmimaggaṁ vītivatto, oḷārikaṁ kāmarāgaṁ, oḷārikaṁ byāpādaṁ vītivatto tena samannāgatoti?",
  "kv4.4:22.4_0": "Na hevaṁ vattabbe …pe….",
  "kv4.4:23.1_0": "Sakadāgāmī sotāpattiphalena samannāgatoti?",
  "kv4.4:23.2_0": "Āmantā.",
  "kv4.4:23.3_0": "Nanu sakadāgāmī sotāpattiphalaṁ vītivattoti?",
  "kv4.4:23.4_0": "Āmantā.",
  "kv4.4:23.5_0": "Hañci sakadāgāmī sotāpattiphalaṁ vītivatto, no ca vata re vattabbe—",
  "kv4.4:23.6_0": "“sakadāgāmī sotāpattiphalena samannāgato”ti.",
  "kv4.4:24.1_0": "Sakadāgāmī sotāpattiphalaṁ vītivatto tena samannāgatoti?",
  "kv4.4:24.2_0": "Āmantā.",
  "kv4.4:24.3_0": "Sakadāgāmī sotāpattimaggaṁ vītivatto, sakkāyadiṭṭhiṁ …pe… apāyagamanīyaṁ mohaṁ vītivatto tena samannāgatoti?",
  "kv4.4:24.4_0": "Na hevaṁ vattabbe …pe….",
  "kv4.4:25.1_0": "Na vattabbaṁ—",
  "kv4.4:25.2_0": "“arahā catūhi phalehi samannāgato”ti?",
  "kv4.4:25.3_0": "Āmantā.",
  "kv4.4:25.4_0": "Nanu arahatā cattāri phalāni paṭiladdhāni tehi ca aparihīnoti?",
  "kv4.4:25.5_0": "Āmantā.",
  "kv4.4:25.6_0": "Hañci arahatā cattāri phalāni paṭiladdhāni tehi ca aparihīno, tena vata re vattabbe—",
  "kv4.4:25.7_0": "“arahā catūhi phalehi samannāgato”ti.",
  "kv4.4:26.1_0": "Na vattabbaṁ—",
  "kv4.4:26.2_0": "“anāgāmī tīhi phalehi samannāgato”ti?",
  "kv4.4:26.3_0": "Āmantā.",
  "kv4.4:26.4_0": "Nanu anāgāminā tīṇi phalāni paṭiladdhāni tehi ca aparihīnoti?",
  "kv4.4:26.5_0": "Āmantā.",
  "kv4.4:26.6_0": "Hañci anāgāminā tīṇi phalāni paṭiladdhāni tehi ca aparihīno, tena vata re vattabbe—",
  "kv4.4:26.7_0": "“anāgāmī tīhi phalehi samannāgato”ti.",
  "kv4.4:27.1_0": "Na vattabbaṁ—",
  "kv4.4:27.2_0": "“sakadāgāmī dvīhi phalehi samannāgato”ti?",
  "kv4.4:27.3_0": "Āmantā.",
  "kv4.4:27.4_0": "Nanu sakadāgāminā dve phalāni paṭiladdhāni tehi ca aparihīnoti?",
  "kv4.4:27.5_0": "Āmantā.",
  "kv4.4:27.6_0": "Hañci sakadāgāminā dve phalāni paṭiladdhāni tehi ca aparihīno, tena vata re vattabbe—",
  "kv4.4:27.7_0": "“sakadāgāmī dvīhi phalehi samannāgato”ti.",
  "kv4.4:28.1_0": "Arahatā cattāri phalāni paṭiladdhāni tehi ca aparihīnoti, arahā catūhi phalehi samannāgatoti?",
  "kv4.4:28.2_0": "Āmantā.",
  "kv4.4:28.3_0": "Arahatā cattāro maggā paṭiladdhā tehi ca aparihīnoti, arahā catūhi maggehi samannāgatoti?",
  "kv4.4:28.4_0": "Na hevaṁ vattabbe …pe….",
  "kv4.4:29.1_0": "Anāgāminā tīṇi phalāni paṭiladdhāni tehi ca aparihīnoti, anāgāmī tīhi phalehi samannāgatoti?",
  "kv4.4:29.2_0": "Āmantā.",
  "kv4.4:29.3_0": "Anāgāminā tayo maggā paṭiladdhā tehi ca aparihīnoti, anāgāmī tīhi maggehi samannāgatoti?",
  "kv4.4:29.4_0": "Na hevaṁ vattabbe …pe….",
  "kv4.4:30.1_0": "Sakadāgāminā dve phalāni paṭiladdhāni tehi ca aparihīnoti, sakadāgāmī dvīhi phalehi samannāgatoti?",
  "kv4.4:30.2_0": "Āmantā.",
  "kv4.4:30.3_0": "Sakadāgāminā dve maggā paṭiladdhā tehi ca aparihīnoti, sakadāgāmī dvīhi maggehi samannāgatoti?",
  "kv4.4:30.4_0": "Na hevaṁ vattabbe …pe….",
  "kv4.4:30.5_0": "Samannāgatakathā niṭṭhitā."
}