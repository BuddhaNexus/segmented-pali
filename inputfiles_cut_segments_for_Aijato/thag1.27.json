{
  "thag1.27:0.1_0": "Theragāthā",
  "thag1.27:0.2_0": "Ekakanipāta",
  "thag1.27:0.3_0": "Tatiyavagga",
  "thag1.27:0.4_0": "1.27. Lomasakaṅgiyattheragāthā",
  "thag1.27:1.1_0": "“Dabbaṁ kusaṁ poṭakilaṁ,",
  "thag1.27:1.2_0": "usīraṁ muñjapabbajaṁ;",
  "thag1.27:1.3_0": "Urasā panudissāmi,",
  "thag1.27:1.4_0": "vivekamanubrūhayan”ti.",
  "thag1.27:1.5_0": "… Lomasakaṅgiyo thero …."
}