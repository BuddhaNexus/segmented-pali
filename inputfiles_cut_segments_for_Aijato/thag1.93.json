{
  "thag1.93:0.1_0": "Theragāthā",
  "thag1.93:0.2_0": "Ekakanipāta",
  "thag1.93:0.3_0": "Dasamavagga",
  "thag1.93:0.4_0": "1.93. Erakattheragāthā",
  "thag1.93:1.1_0": "“Dukkhā kāmā eraka,",
  "thag1.93:1.2_0": "Na sukhā kāmā eraka;",
  "thag1.93:1.3_0": "Yo kāme kāmayati,",
  "thag1.93:1.4_0": "Dukkhaṁ so kāmayati eraka;",
  "thag1.93:1.5_0": "Yo kāme na kāmayati,",
  "thag1.93:1.6_0": "Dukkhaṁ so na kāmayati erakā”ti.",
  "thag1.93:1.7_0": "… Erako thero …."
}