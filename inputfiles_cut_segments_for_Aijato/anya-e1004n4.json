{
  "anya-e1004n4:0_0": "Itthikathā",
  "anya-e1004n4:1_0": "198.",
  "anya-e1004n4:2_0": "Namātarā dhītuyā vāpi,",
  "anya-e1004n4:3_0": "Bhagīniyā vicakkhaṇo;",
  "anya-e1004n4:4_0": "Navivittāsane mante,",
  "anya-e1004n4:5_0": "Nārī māyāvinīnanu.",
  "anya-e1004n4:6_0": "199.",
  "anya-e1004n4:7_0": "Vijjutānañca lolattaṁ,",
  "anya-e1004n4:8_0": "Satthānañcatitikkhaṇaṁ;",
  "anya-e1004n4:9_0": "Sīghataṁ vāyutejānaṁ,",
  "anya-e1004n4:10_0": "Anukubbanti nāriyo.",
  "anya-e1004n4:11_0": "200.",
  "anya-e1004n4:12_0": "Dviguṇo thīna māhāro,",
  "anya-e1004n4:13_0": "Buddhicāpi catugguṇo;",
  "anya-e1004n4:14_0": "Chaguṇo hoti vāyāmo,",
  "anya-e1004n4:15_0": "Kāmotva ṭṭha guṇobhave.",
  "anya-e1004n4:16_0": "201.",
  "anya-e1004n4:17_0": "Ekamekāyaitthīyā,",
  "anya-e1004n4:18_0": "Aṭṭhaṭṭhapatino siyuṁ;",
  "anya-e1004n4:19_0": "Sūrāca balavantoca,",
  "anya-e1004n4:20_0": "Sabbākāmarasāharā;",
  "anya-e1004n4:21_0": "Kareyya navame chandaṁ,",
  "anya-e1004n4:22_0": "Unnattā hi napūrati.",
  "anya-e1004n4:23_0": "202.",
  "anya-e1004n4:24_0": "Lapanti saddhimaññena,",
  "anya-e1004n4:25_0": "Passantaññaṁ savibbhamā;",
  "anya-e1004n4:26_0": "Cittakaṁ cintayantaññaṁ,",
  "anya-e1004n4:27_0": "Nārīnaṁnāma ko piyo.",
  "anya-e1004n4:28_0": "203.",
  "anya-e1004n4:29_0": "Jivhāsahassiko yo hi,",
  "anya-e1004n4:30_0": "Jīve vassasataṁ naro;",
  "anya-e1004n4:31_0": "Tena nikammuno vutto,",
  "anya-e1004n4:32_0": "Thīdoso kiṁkhayaṁgato.",
  "anya-e1004n4:33_0": "204.",
  "anya-e1004n4:34_0": "Aggi āpo thiyo muḷho,",
  "anya-e1004n4:35_0": "Sappo rājakulānica;",
  "anya-e1004n4:36_0": "Payatanāpagantabbo,",
  "anya-e1004n4:37_0": "Maccupāṇaharāniti.",
  "anya-e1004n4:38_0": "205.",
  "anya-e1004n4:39_0": "Itthīnaṁ dujjanā nañca,",
  "anya-e1004n4:40_0": "Visāsonopapajjate;",
  "anya-e1004n4:41_0": "Vīsassa siṅgino roga,",
  "anya-e1004n4:42_0": "Nadīrājakulassaca.",
  "anya-e1004n4:43_0": "206.",
  "anya-e1004n4:44_0": "Satthaṁ sunicchitamatīpi vicintanīyaṁ,",
  "anya-e1004n4:45_0": "Svārādhitopya vanipo parisaṅkanīyo;",
  "anya-e1004n4:46_0": "Hatthaṅgatāpi yuvatīparirakkhanīyā,",
  "anya-e1004n4:47_0": "Satthāvanīpayuvatīsu kutovasitthaṁ.",
  "anya-e1004n4:48_0": "207.",
  "anya-e1004n4:49_0": "Ayuttakammāraddhanaṁ virodho,",
  "anya-e1004n4:50_0": "Saṅghassa yuddhañca mahābalehi;",
  "anya-e1004n4:51_0": "Vissāsakammaṁ pamādāsu niccaṁ,",
  "anya-e1004n4:52_0": "Dvārānimaccussa vadanti paññā.",
  "anya-e1004n4:53_0": "208.",
  "anya-e1004n4:54_0": "Vātaṁ jālena naro parāmase,",
  "anya-e1004n4:55_0": "Osiñce sāgaraṁ ekapāṇinā;",
  "anya-e1004n4:56_0": "Sakenatālena janeyya ghosaṁ,",
  "anya-e1004n4:57_0": "Yo sabbabhāvaṁ pamadāsu osaje.",
  "anya-e1004n4:58_0": "209.",
  "anya-e1004n4:59_0": "Itthīpi hi ekacciyā,",
  "anya-e1004n4:60_0": "Seyyā vuttāva muninā;",
  "anya-e1004n4:61_0": "Bhaṇḍāna muttamaṁ itthī,",
  "anya-e1004n4:62_0": "Aggupaṭṭhāyikātica.",
  "anya-e1004n4:63_0": "210.",
  "anya-e1004n4:64_0": "Yo naṁ bharati sabbadā,",
  "anya-e1004n4:65_0": "Niccaṁ ātāpi ussuko;",
  "anya-e1004n4:66_0": "Sabbakāmaharaṁ posaṁ,",
  "anya-e1004n4:67_0": "Bhattāraṁ nātimaññati.",
  "anya-e1004n4:68_0": "211.",
  "anya-e1004n4:69_0": "Nācāpi sotthi bhattāraṁ,",
  "anya-e1004n4:70_0": "Issācārena rosaye;",
  "anya-e1004n4:71_0": "Bhattuca garunosabbe,",
  "anya-e1004n4:72_0": "Paṭipūjeti paṇḍito.",
  "anya-e1004n4:73_0": "212.",
  "anya-e1004n4:74_0": "Uṭṭhāyikā anālāsā,",
  "anya-e1004n4:75_0": "Saṅgahitaparijjanā;",
  "anya-e1004n4:76_0": "Bhattu manāpaṁ carati,",
  "anya-e1004n4:77_0": "Bhattakaṁ anurakkhati.",
  "anya-e1004n4:78_0": "213.",
  "anya-e1004n4:79_0": "Evaṁ vattati yānārī,",
  "anya-e1004n4:80_0": "Bhattuchandavasānugā;",
  "anya-e1004n4:81_0": "Manāpānāma tedevā,",
  "anya-e1004n4:82_0": "Yattha sā upapajjati.",
  "anya-e1004n4:83_0": "214.",
  "anya-e1004n4:84_0": "Kokilānaṁ saro rūpaṁ,",
  "anya-e1004n4:85_0": "Nārīrūpaṁ patibbataṁ;",
  "anya-e1004n4:86_0": "Vijjā rūpa marūpānaṁ,",
  "anya-e1004n4:87_0": "Khamā rūpaṁ tapassīnaṁ.",
  "anya-e1004n4:88_0": "215.",
  "anya-e1004n4:89_0": "Āneya kulajaṁ pañño,",
  "anya-e1004n4:90_0": "Virūpamapi kaññakaṁ;",
  "anya-e1004n4:91_0": "Hīnāyapi surūpāya,",
  "anya-e1004n4:92_0": "Vivāhaṁsadisaṁ kare.",
  "anya-e1004n4:93_0": "216.",
  "anya-e1004n4:94_0": "Visamhāmatamādeyya ,",
  "anya-e1004n4:95_0": "Amajjamhāpikañcanaṁ;",
  "anya-e1004n4:96_0": "Nicamhāpyuttamāvijjā,",
  "anya-e1004n4:97_0": "Ratanitthīpidukulā.",
  "anya-e1004n4:98_0": "217.",
  "anya-e1004n4:99_0": "Bālitthī makkhikātuṇḍaṁ,",
  "anya-e1004n4:100_0": "Isīnañca kamaṇḍaluṁ;",
  "anya-e1004n4:101_0": "Setambu phalaṁ tambulaṁ,",
  "anya-e1004n4:102_0": "Nocchiṭṭhamupajāyate.",
  "anya-e1004n4:103_0": "218.",
  "anya-e1004n4:104_0": "Bālakko paṇṇadhūmoca,",
  "anya-e1004n4:105_0": "Vuddhitthī pallalodakaṁ;",
  "anya-e1004n4:106_0": "Āyukkhayakaraṁ niccaṁ,",
  "anya-e1004n4:107_0": "Rattoca dadhibhojanaṁ.",
  "anya-e1004n4:108_0": "219.",
  "anya-e1004n4:109_0": "Thiyo seveyya naccantaṁ,",
  "anya-e1004n4:110_0": "Sāduṁ bhuñjeyyanāhitaṁ;",
  "anya-e1004n4:111_0": "Pūjaye mānaye vuddhe,",
  "anya-e1004n4:112_0": "Guruṁ māyāya nobhaje.",
  "anya-e1004n4:113_0": "220.",
  "anya-e1004n4:114_0": "Ācāro kula mākhyāti,",
  "anya-e1004n4:115_0": "Desa mākhyāti bhāsitaṁ;",
  "anya-e1004n4:116_0": "Sambhavo pema mākhyāti,",
  "anya-e1004n4:117_0": "Deha mākhyātibhojanaṁ.",
  "anya-e1004n4:118_0": "221.",
  "anya-e1004n4:119_0": "Dehītivacanādvārā ,",
  "anya-e1004n4:120_0": "Dehaṭṭhāpañcadevatā;",
  "anya-e1004n4:121_0": "Sajja niyanti dhī kitti,",
  "anya-e1004n4:122_0": "Hirī sirī matīpica.",
  "anya-e1004n4:123_0": "222.",
  "anya-e1004n4:124_0": "Dehītivacanaṁ dukkhaṁ,",
  "anya-e1004n4:125_0": "Natthītivacanaṁ tathā;",
  "anya-e1004n4:126_0": "Vākyaṁdehītinatthīti,",
  "anya-e1004n4:127_0": "Mābhaveyya bhavebhave.",
  "anya-e1004n4:128_0": "223.",
  "anya-e1004n4:129_0": "Bodhayanti nayācanti,",
  "anya-e1004n4:130_0": "Dehīti pacchimājanā;",
  "anya-e1004n4:131_0": "Passa vatthumadānassa,",
  "anya-e1004n4:132_0": "Mābhavatūti īdiso.",
  "anya-e1004n4:133_0": "224.",
  "anya-e1004n4:134_0": "Mahā atyappakaṁ yāti,",
  "anya-e1004n4:135_0": "Niguṇe guṇavāpiha;",
  "anya-e1004n4:136_0": "Aṭṭhānaṭṭheyyabhāvena,",
  "anya-e1004n4:137_0": "Gajindoiva dabbake.",
  "anya-e1004n4:138_0": "225.",
  "anya-e1004n4:139_0": "Mahantaṁ nissayaṁkatvā,",
  "anya-e1004n4:140_0": "Khuddopyatimahā bhave;",
  "anya-e1004n4:141_0": "Hemapabbatamāpajja,",
  "anya-e1004n4:142_0": "Sovaṇṇākira pakkhino.",
  "anya-e1004n4:143_0": "226.",
  "anya-e1004n4:144_0": "Bahūna mappasārānaṁ,",
  "anya-e1004n4:145_0": "Ekibhāvo hidujjayo;",
  "anya-e1004n4:146_0": "Tiṇena vaṭṭate rajju,",
  "anya-e1004n4:147_0": "Tāya nāgopi bandhate.",
  "anya-e1004n4:148_0": "227.",
  "anya-e1004n4:149_0": "Asahāyo samatthopi,",
  "anya-e1004n4:150_0": "Tejasi kiṁ karissati;",
  "anya-e1004n4:151_0": "Nivātasaṇṭhito aggi,",
  "anya-e1004n4:152_0": "Sayamevūpasammati.",
  "anya-e1004n4:153_0": "228.",
  "anya-e1004n4:154_0": "Khantuṁ tapanajotejo,",
  "anya-e1004n4:155_0": "Sakkāhoti na vaṇṇajo;",
  "anya-e1004n4:156_0": "Bhūpādīhi katodaṇḍo,",
  "anya-e1004n4:157_0": "Sakkāhoti na bhaccajo.",
  "anya-e1004n4:158_0": "229.",
  "anya-e1004n4:159_0": "Thīsaṁsagge kutosuddho,",
  "anya-e1004n4:160_0": "Maṁsabhakkhe kutodayā;",
  "anya-e1004n4:161_0": "Surāpāṇe kutosaccaṁ,",
  "anya-e1004n4:162_0": "Pakodhamhi kutotapo.",
  "anya-e1004n4:163_0": "230.",
  "anya-e1004n4:164_0": "Thīyā guyhaṁ nasaṁseyya,",
  "anya-e1004n4:165_0": "Amittassaca paṇḍito;",
  "anya-e1004n4:166_0": "Yocā missena saṁhīro,",
  "anya-e1004n4:167_0": "Hadayattheno yonaro.",
  "anya-e1004n4:168_0": "231.",
  "anya-e1004n4:169_0": "Guyhamattha ",
  "anya-e1004n4:170_0": "Sambodhayati yonaro;",
  "anya-e1004n4:171_0": "Mantabhedabhayā tassa,",
  "anya-e1004n4:172_0": "Dāsabhūto titikkhati.",
  "anya-e1004n4:173_0": "232.",
  "anya-e1004n4:174_0": "Vahe amitta khandhena,",
  "anya-e1004n4:175_0": "Yāvakāle anāgate;",
  "anya-e1004n4:176_0": "Tamheva cāgate kāle,",
  "anya-e1004n4:177_0": "Bhinde ghaṭami vuppale.",
  "anya-e1004n4:178_0": "233.",
  "anya-e1004n4:179_0": "Khalaṁ sālaṁ pasuṁ khettaṁ,",
  "anya-e1004n4:180_0": "Gantvācassa abhikkhaṇaṁ;",
  "anya-e1004n4:181_0": "Mitaṁ dhaññaṁ nidhāpeyya,",
  "anya-e1004n4:182_0": "Mitañca pācaye ghare.",
  "anya-e1004n4:183_0": "234.",
  "anya-e1004n4:184_0": "Kodhaṁ lobhaṁ madaṁ mānaṁ,",
  "anya-e1004n4:185_0": "Tandiṁ missaṁ pamattakaṁ;",
  "anya-e1004n4:186_0": "Soṇṭhaṁniddhālukaṁ makkhaṁ,",
  "anya-e1004n4:187_0": "Maccherañca jahe budho.",
  "anya-e1004n4:188_0": "235.",
  "anya-e1004n4:189_0": "Kodho abbhantare jāto,",
  "anya-e1004n4:190_0": "Dhūvaṁ nāseti kodhanaṁ;",
  "anya-e1004n4:191_0": "Vatthālaṅkārapuṇṇāyaṁ,",
  "anya-e1004n4:192_0": "Mañjūsāyaṁ sikhīyathā.",
  "anya-e1004n4:193_0": "236.",
  "anya-e1004n4:194_0": "Uppajjate sace kodho;",
  "anya-e1004n4:195_0": "Āvajje kakacūpamaṁ;",
  "anya-e1004n4:196_0": "Uppajje ce rase taṇhā,",
  "anya-e1004n4:197_0": "Puttamaṁsūpamaṁ sare.",
  "anya-e1004n4:198_0": "237.",
  "anya-e1004n4:199_0": "Guṇa maddhisamaṁ makkhe,",
  "anya-e1004n4:200_0": "Parena kalahe sati;",
  "anya-e1004n4:201_0": "Addhisamaṁ pakāseti,",
  "anya-e1004n4:202_0": "Aṇumattaṁvavajjakaṁ.",
  "anya-e1004n4:203_0": "238.",
  "anya-e1004n4:204_0": "Tasseva tena pāpīyo,",
  "anya-e1004n4:205_0": "Yo kuddhaṁ paṭikujjhati;",
  "anya-e1004n4:206_0": "Kuddha mapaṭikujjhanto,",
  "anya-e1004n4:207_0": "Saṅgāmaṁ jeti dujjayaṁ.",
  "anya-e1004n4:208_0": "239.",
  "anya-e1004n4:209_0": "Rāgonāma manosallaṁ,",
  "anya-e1004n4:210_0": "Guṇatthavaratakkaro;",
  "anya-e1004n4:211_0": "Rāhu vijjāsasaṅkassa,",
  "anya-e1004n4:212_0": "Tapodhanahutāsano.",
  "anya-e1004n4:213_0": "240.",
  "anya-e1004n4:214_0": "Pamādo jāyate madā,",
  "anya-e1004n4:215_0": "Pamādā jāyate khayo;",
  "anya-e1004n4:216_0": "Khayā padosā jāyanti,",
  "anya-e1004n4:217_0": "Madaṁ kiṁ najahe budho.",
  "anya-e1004n4:218_0": "241.",
  "anya-e1004n4:219_0": "Namanti phalinorukkhā,",
  "anya-e1004n4:220_0": "Namanti vibudhājanā;",
  "anya-e1004n4:221_0": "Sukkhakaṭṭhañca muḷhoca,",
  "anya-e1004n4:222_0": "Bhijjateva nanamanti.",
  "anya-e1004n4:223_0": "242.",
  "anya-e1004n4:224_0": "Ṭhāne vuddhāna mokāsaṁ,",
  "anya-e1004n4:225_0": "Dade vuddhāpacāyiko;",
  "anya-e1004n4:226_0": "Nanu tālo ajīvopi,",
  "anya-e1004n4:227_0": "Samīpa ññe paronato.",
  "anya-e1004n4:228_0": "243.",
  "anya-e1004n4:229_0": "Garukātabbaposesu,",
  "anya-e1004n4:230_0": "Niccavuttiṁ karoti yo;",
  "anya-e1004n4:231_0": "Nicattaṁ so pahantāna,",
  "anya-e1004n4:232_0": "Uttamatte patiṭṭhati.",
  "anya-e1004n4:233_0": "244.",
  "anya-e1004n4:234_0": "Yattha posaṁ najānanti,",
  "anya-e1004n4:235_0": "Jātiyā vinayena vā;",
  "anya-e1004n4:236_0": "Na tattha mānaṁ kariyā,",
  "anya-e1004n4:237_0": "Vasa maññātakejane.",
  "anya-e1004n4:238_0": "245.",
  "anya-e1004n4:239_0": "Aññātavāsaṁ vasatā,",
  "anya-e1004n4:240_0": "Jātavedasamenāpi;",
  "anya-e1004n4:241_0": "Khamitabbaṁ sapaññena,",
  "anya-e1004n4:242_0": "Api dāsassa tajjitaṁ.",
  "anya-e1004n4:243_0": "246.",
  "anya-e1004n4:244_0": "Dhanadhaññāpayogesu,",
  "anya-e1004n4:245_0": "Tathā vijjāgamesuca;",
  "anya-e1004n4:246_0": "Dūteca byavahāreca,",
  "anya-e1004n4:247_0": "Cattalajjo sadā bhave.",
  "anya-e1004n4:248_0": "247.",
  "anya-e1004n4:249_0": "Nahi koci kate kicce,",
  "anya-e1004n4:250_0": "Kattāraṁ sammapekkhate;",
  "anya-e1004n4:251_0": "Tasmā sabbānikammāni,",
  "anya-e1004n4:252_0": "Sāvasesāni kāraye.",
  "anya-e1004n4:253_0": "248.",
  "anya-e1004n4:254_0": "Iṇaseso aggiseso,",
  "anya-e1004n4:255_0": "Sattuseso tayoime;",
  "anya-e1004n4:256_0": "Punappunaṁpi vaḍḍhanti,",
  "anya-e1004n4:257_0": "Tasmā sesaṁ nakāraye.",
  "anya-e1004n4:258_0": "249.",
  "anya-e1004n4:259_0": "Natthi vijjāsamaṁ vittaṁ,",
  "anya-e1004n4:260_0": "Natthi byādhisamo ripu;",
  "anya-e1004n4:261_0": "Natthi attasamaṁ pemaṁ,",
  "anya-e1004n4:262_0": "Natthi kammaparaṁ balaṁ.",
  "anya-e1004n4:263_0": "250.",
  "anya-e1004n4:264_0": "Attanā kurute lakkhī,",
  "anya-e1004n4:265_0": "Alakkhīcāpi attanā;",
  "anya-e1004n4:266_0": "Nahi lakkhī alakkhīca,",
  "anya-e1004n4:267_0": "Añño aññassa kurute.",
  "anya-e1004n4:268_0": "251.",
  "anya-e1004n4:269_0": "Sayaṁ āyaṁ vayaṁ raññā,",
  "anya-e1004n4:270_0": "Sayaṁ jaññā katākataṁ;",
  "anya-e1004n4:271_0": "Attanāva bhavakkheyya,",
  "anya-e1004n4:272_0": "Katāni akatānica.",
  "anya-e1004n4:273_0": "252.",
  "anya-e1004n4:274_0": "Upakāraṁ hiteneva,",
  "anya-e1004n4:275_0": "Sattunā sattu muddhare;",
  "anya-e1004n4:276_0": "Pādalaggaṁ karaṭṭhena,",
  "anya-e1004n4:277_0": "Kaṇṭakena kaṇṭakaṁ.",
  "anya-e1004n4:278_0": "253.",
  "anya-e1004n4:279_0": "Name namantassa bhaje bhajantaṁ,",
  "anya-e1004n4:280_0": "Kiccāni krupassa kareyya kiccaṁ;",
  "anya-e1004n4:281_0": "Nā natthakāmassa kareyya atthaṁ,",
  "anya-e1004n4:282_0": "Asambhajantaṁpi nasambhajeyya.",
  "anya-e1004n4:283_0": "254.",
  "anya-e1004n4:284_0": "Caje cajantaṁ navataṁ kariyā,",
  "anya-e1004n4:285_0": "Apetacittena nasambhajeyya;",
  "anya-e1004n4:286_0": "Dijo dumaṁ khīṇaphala mañatvā,",
  "anya-e1004n4:287_0": "Aññaṁ sapekkheyya mahāti loko."
}