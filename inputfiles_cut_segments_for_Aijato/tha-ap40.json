{
  "tha-ap40:0.1_0": "Therāpadāna",
  "tha-ap40:0.2_0": "Kuṇḍadhānavagga",
  "tha-ap40:0.3_0": "8 Āyāgadāyakattheraapadāna",
  "tha-ap40:1.1_0": "“Nibbute lokanāthamhi,",
  "tha-ap40:1.2_0": "sikhimhi vadataṁ vare;",
  "tha-ap40:1.3_0": "Haṭṭho haṭṭhena cittena,",
  "tha-ap40:1.4_0": "avandiṁ thūpamuttamaṁ.",
  "tha-ap40:2.1_0": "Vaḍḍhakīhi kathāpetvā,",
  "tha-ap40:2.2_0": "mūlaṁ datvānahaṁ tadā;",
  "tha-ap40:2.3_0": "Haṭṭho haṭṭhena cittena,",
  "tha-ap40:2.4_0": "āyāgaṁ kārapesahaṁ.",
  "tha-ap40:3.1_0": "Aṭṭha kappāni devesu,",
  "tha-ap40:3.2_0": "abbokiṇṇaṁ vasiṁ ahaṁ;",
  "tha-ap40:3.3_0": "Avasesesu kappesu,",
  "tha-ap40:3.4_0": "vokiṇṇaṁ saṁsariṁ ahaṁ.",
  "tha-ap40:4.1_0": "Kāye visaṁ na kamati,",
  "tha-ap40:4.2_0": "satthāni na ca hanti me;",
  "tha-ap40:4.3_0": "Udakehaṁ na miyyāmi,",
  "tha-ap40:4.4_0": "āyāgassa idaṁ phalaṁ.",
  "tha-ap40:5.1_0": "Yadicchāmi ahaṁ vassaṁ,",
  "tha-ap40:5.2_0": "mahāmegho pavassati;",
  "tha-ap40:5.3_0": "Devāpi me vasaṁ enti,",
  "tha-ap40:5.4_0": "puññakammassidaṁ phalaṁ.",
  "tha-ap40:6.1_0": "Sattaratanasampanno,",
  "tha-ap40:6.2_0": "tiṁsakkhattuṁ ahosahaṁ;",
  "tha-ap40:6.3_0": "Na maṁ kecāvajānanti,",
  "tha-ap40:6.4_0": "puññakammassidaṁ phalaṁ.",
  "tha-ap40:7.1_0": "Ekattiṁse ito kappe,",
  "tha-ap40:7.2_0": "āyāgaṁ yamakārayiṁ;",
  "tha-ap40:7.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap40:7.4_0": "āyāgassa idaṁ phalaṁ.",
  "tha-ap40:8.1_0": "Paṭisambhidā catasso,",
  "tha-ap40:8.2_0": "…pe…",
  "tha-ap40:8.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap40:9.1_0": "Itthaṁ sudaṁ āyasmā āyāgadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap40:10.1_0": "Āyāgadāyakattherassāpadānaṁ aṭṭhamaṁ."
}