{
  "tha-ap114:0.1_0": "Therāpadāna",
  "tha-ap114:0.2_0": "Mahāparivāravagga",
  "tha-ap114:0.3_0": "2. Sumaṅgalattheraapadāna",
  "tha-ap114:1.1_0": "“Atthadassī jinavaro,",
  "tha-ap114:1.2_0": "lokajeṭṭho narāsabho;",
  "tha-ap114:1.3_0": "Vihārā abhinikkhamma,",
  "tha-ap114:1.4_0": "taḷākaṁ upasaṅkami.",
  "tha-ap114:2.1_0": "Nhatvā pitvā ca sambuddho,",
  "tha-ap114:2.2_0": "uttaritvekacīvaro;",
  "tha-ap114:2.3_0": "Aṭṭhāsi bhagavā tattha,",
  "tha-ap114:2.4_0": "vilokento disodisaṁ.",
  "tha-ap114:3.1_0": "Bhavane upaviṭṭhohaṁ,",
  "tha-ap114:3.2_0": "addasaṁ lokanāyakaṁ;",
  "tha-ap114:3.3_0": "Haṭṭho haṭṭhena cittena,",
  "tha-ap114:3.4_0": "apphoṭesiṁ ahaṁ tadā.",
  "tha-ap114:4.1_0": "Sataraṁsiṁva jotantaṁ,",
  "tha-ap114:4.2_0": "pabhāsantaṁva kañcanaṁ;",
  "tha-ap114:4.3_0": "Naccagīte payuttohaṁ,",
  "tha-ap114:4.4_0": "pañcaṅgatūriyamhi ca.",
  "tha-ap114:5.1_0": "Yaṁ yaṁ yonupapajjāmi,",
  "tha-ap114:5.2_0": "devattaṁ atha mānusaṁ;",
  "tha-ap114:5.3_0": "Sabbe satte abhibhomi,",
  "tha-ap114:5.4_0": "vipulo hoti me yaso.",
  "tha-ap114:6.1_0": "Namo te purisājañña,",
  "tha-ap114:6.2_0": "namo te purisuttama;",
  "tha-ap114:6.3_0": "Attānaṁ tosayitvāna,",
  "tha-ap114:6.4_0": "pare tosesi tvaṁ muni.",
  "tha-ap114:7.1_0": "Pariggahe nisīditvā,",
  "tha-ap114:7.2_0": "hāsaṁ katvāna subbate;",
  "tha-ap114:7.3_0": "Upaṭṭhahitvā sambuddhaṁ,",
  "tha-ap114:7.4_0": "tusitaṁ upapajjahaṁ.",
  "tha-ap114:8.1_0": "Soḷaseto kappasate,",
  "tha-ap114:8.2_0": "dvinavaekacintitā;",
  "tha-ap114:8.3_0": "Sattaratanasampannā,",
  "tha-ap114:8.4_0": "cakkavattī mahabbalā.",
  "tha-ap114:9.1_0": "Paṭisambhidā catasso,",
  "tha-ap114:9.2_0": "…pe…",
  "tha-ap114:9.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap114:10.1_0": "Itthaṁ sudaṁ āyasmā sumaṅgalo thero imā gāthāyo abhāsitthāti.",
  "tha-ap114:11.1_0": "Sumaṅgalattherassāpadānaṁ dutiyaṁ."
}