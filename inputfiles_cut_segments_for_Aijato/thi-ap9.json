{
  "thi-ap9:0.1_0": "Therīapadāna",
  "thi-ap9:0.2_0": "Sumedhāvagga",
  "thi-ap9:0.3_0": "9. Pañcadīpikātherīapadāna",
  "thi-ap9:1.1_0": "“Nagare haṁsavatiyā,",
  "thi-ap9:1.2_0": "cārikī āsahaṁ tadā;",
  "thi-ap9:1.3_0": "Ārāmena ca ārāmaṁ,",
  "thi-ap9:1.4_0": "carāmi kusalatthikā.",
  "thi-ap9:2.1_0": "Kāḷapakkhamhi divase,",
  "thi-ap9:2.2_0": "addasaṁ bodhimuttamaṁ;",
  "thi-ap9:2.3_0": "Tattha cittaṁ pasādetvā,",
  "thi-ap9:2.4_0": "bodhimūle nisīdahaṁ.",
  "thi-ap9:3.1_0": "Garucittaṁ upaṭṭhetvā,",
  "thi-ap9:3.2_0": "sire katvāna añjaliṁ;",
  "thi-ap9:3.3_0": "Somanassaṁ pavedetvā,",
  "thi-ap9:3.4_0": "evaṁ cintesi tāvade.",
  "thi-ap9:4.1_0": "‘Yadi buddho amitaguṇo,",
  "thi-ap9:4.2_0": "asamappaṭipuggalo;",
  "thi-ap9:4.3_0": "Dassetu pāṭihīraṁ me,",
  "thi-ap9:4.4_0": "bodhi obhāsatu ayaṁ’.",
  "thi-ap9:5.1_0": "Saha āvajjite mayhaṁ,",
  "thi-ap9:5.2_0": "bodhi pajjali tāvade;",
  "thi-ap9:5.3_0": "Sabbasoṇṇamayā āsi,",
  "thi-ap9:5.4_0": "disā sabbā virocati.",
  "thi-ap9:6.1_0": "Sattarattindivaṁ tattha,",
  "thi-ap9:6.2_0": "bodhimūle nisīdahaṁ;",
  "thi-ap9:6.3_0": "Sattame divase patte,",
  "thi-ap9:6.4_0": "dīpapūjaṁ akāsahaṁ.",
  "thi-ap9:7.1_0": "Āsanaṁ parivāretvā,",
  "thi-ap9:7.2_0": "pañca dīpāni pajjaluṁ;",
  "thi-ap9:7.3_0": "Yāva udeti sūriyo,",
  "thi-ap9:7.4_0": "dīpā me pajjaluṁ tadā.",
  "thi-ap9:8.1_0": "Tena kammena sukatena,",
  "thi-ap9:8.2_0": "cetanāpaṇidhīhi ca;",
  "thi-ap9:8.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "thi-ap9:8.4_0": "tāvatiṁsamagacchahaṁ.",
  "thi-ap9:9.1_0": "Tattha me sukataṁ byamhaṁ,",
  "thi-ap9:9.2_0": "pañcadīpāti vuccati;",
  "thi-ap9:9.3_0": "Saṭṭhiyojanamubbedhaṁ,",
  "thi-ap9:9.4_0": "tiṁsayojanavitthataṁ.",
  "thi-ap9:10.1_0": "Asaṅkhiyāni dīpāni,",
  "thi-ap9:10.2_0": "parivāre jalanti me;",
  "thi-ap9:10.3_0": "Yāvatā devabhavanaṁ,",
  "thi-ap9:10.4_0": "dīpālokena jotati.",
  "thi-ap9:11.1_0": "Parammukhā nisīditvā,",
  "thi-ap9:11.2_0": "yadi icchāmi passituṁ;",
  "thi-ap9:11.3_0": "Uddhaṁ adho ca tiriyaṁ,",
  "thi-ap9:11.4_0": "sabbaṁ passāmi cakkhunā.",
  "thi-ap9:12.1_0": "Yāvatā abhikaṅkhāmi,",
  "thi-ap9:12.2_0": "daṭṭhuṁ sugataduggate;",
  "thi-ap9:12.3_0": "Tattha āvaraṇaṁ natthi,",
  "thi-ap9:12.4_0": "rukkhesu pabbatesu vā.",
  "thi-ap9:13.1_0": "Asīti devarājūnaṁ,",
  "thi-ap9:13.2_0": "mahesittamakārayiṁ;",
  "thi-ap9:13.3_0": "Satānaṁ cakkavattīnaṁ,",
  "thi-ap9:13.4_0": "mahesittamakārayiṁ.",
  "thi-ap9:14.1_0": "Yaṁ yaṁ yonupapajjāmi,",
  "thi-ap9:14.2_0": "devattaṁ atha mānusaṁ;",
  "thi-ap9:14.3_0": "Dīpasatasahassāni,",
  "thi-ap9:14.4_0": "parivāre jalanti me.",
  "thi-ap9:15.1_0": "Devalokā cavitvāna,",
  "thi-ap9:15.2_0": "uppajjiṁ mātukucchiyaṁ;",
  "thi-ap9:15.3_0": "Mātukucchigatā santī,",
  "thi-ap9:15.4_0": "akkhi me na nimīlati.",
  "thi-ap9:16.1_0": "Dīpasatasahassāni,",
  "thi-ap9:16.2_0": "puññakammasamaṅgitā;",
  "thi-ap9:16.3_0": "Jalanti sūtikāgehe,",
  "thi-ap9:16.4_0": "pañcadīpānidaṁ phalaṁ.",
  "thi-ap9:17.1_0": "Pacchime bhave sampatte,",
  "thi-ap9:17.2_0": "mānasaṁ vinivattayiṁ;",
  "thi-ap9:17.3_0": "Ajarāmataṁ sītibhāvaṁ,",
  "thi-ap9:17.4_0": "nibbānaṁ phassayiṁ ahaṁ.",
  "thi-ap9:18.1_0": "Jātiyā sattavassāhaṁ,",
  "thi-ap9:18.2_0": "arahattamapāpuṇiṁ;",
  "thi-ap9:18.3_0": "Upasampādayī buddho,",
  "thi-ap9:18.4_0": "guṇamaññāya gotamo.",
  "thi-ap9:19.1_0": "Maṇḍape rukkhamūle vā,",
  "thi-ap9:19.2_0": "pāsādesu guhāsu vā;",
  "thi-ap9:19.3_0": "Suññāgāre vasantiyā,",
  "thi-ap9:19.4_0": "pañcadīpā jalanti me.",
  "thi-ap9:20.1_0": "Dibbacakkhuvisuddhaṁ me,",
  "thi-ap9:20.2_0": "samādhikusalā ahaṁ;",
  "thi-ap9:20.3_0": "Abhiññāpāramippattā,",
  "thi-ap9:20.4_0": "pañcadīpānidaṁ phalaṁ.",
  "thi-ap9:21.1_0": "Sabbavositavosānā,",
  "thi-ap9:21.2_0": "katakiccā anāsavā;",
  "thi-ap9:21.3_0": "Pañcadīpā mahāvīra,",
  "thi-ap9:21.4_0": "pāde vandāmi cakkhuma.",
  "thi-ap9:22.1_0": "Satasahassito kappe,",
  "thi-ap9:22.2_0": "yaṁ dīpamadadiṁ tadā;",
  "thi-ap9:22.3_0": "Duggatiṁ nābhijānāmi,",
  "thi-ap9:22.4_0": "pañcadīpānidaṁ phalaṁ.",
  "thi-ap9:23.1_0": "Kilesā jhāpitā mayhaṁ,",
  "thi-ap9:23.2_0": "…pe…",
  "thi-ap9:23.3_0": "viharāmi anāsavā.",
  "thi-ap9:24.1_0": "Svāgataṁ vata me āsi,",
  "thi-ap9:24.2_0": "…pe…",
  "thi-ap9:24.3_0": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap9:25.1_0": "Paṭisambhidā catasso,",
  "thi-ap9:25.2_0": "…pe…",
  "thi-ap9:25.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "thi-ap9:26.1_0": "Itthaṁ sudaṁ pañcadīpikā bhikkhunī imā gāthāyo abhāsitthāti.",
  "thi-ap9:27.1_0": "Pañcadīpikātheriyāpadānaṁ navamaṁ."
}