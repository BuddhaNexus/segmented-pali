{
  "ja321:0.1_0": "Jātaka",
  "ja321:0.2_0": "Catukkanipāta",
  "ja321:0.3_0": "Kuṭidūsakavagga",
  "ja321:0.4_0": "1. Kuṭidūsakajātaka",
  "ja321:1.1_0": "“Manussasseva te sīsaṁ,",
  "ja321:1.2_0": "hatthapādā ca vānara;",
  "ja321:1.3_0": "Atha kena nu vaṇṇena,",
  "ja321:1.4_0": "agāraṁ te na vijjati”.",
  "ja321:2.1_0": "“Manussasseva me sīsaṁ,",
  "ja321:2.2_0": "hatthapādā ca siṅgila;",
  "ja321:2.3_0": "Yāhu seṭṭhā manussesu,",
  "ja321:2.4_0": "sā me paññā na vijjati”.",
  "ja321:3.1_0": "“Anavaṭṭhitacittassa,",
  "ja321:3.2_0": "lahucittassa dubbhino;",
  "ja321:3.3_0": "Niccaṁ addhuvasīlassa,",
  "ja321:3.4_0": "sukhabhāvo na vijjati.",
  "ja321:4.1_0": "So karassu ānubhāvaṁ,",
  "ja321:4.2_0": "vītivattassu sīliyaṁ;",
  "ja321:4.3_0": "Sītavātaparittāṇaṁ,",
  "ja321:4.4_0": "karassu kuṭavaṁ kapī”ti.",
  "ja321:5.1_0": "Kuṭidūsakajātakaṁ paṭhamaṁ."
}