{
  "atk-s0404a30:0_0": "(22) 2. Sāmaññavaggavaṇṇanā",
  "atk-s0404a30:1_0": "221.",
  "atk-s0404a30:1_1": "Dutiyassa paṭhamaṁ ādiṁ katvā sabbā peyyālatanti uttānatthāyevāti.",
  "atk-s0404a30:2_0": "Manorathapūraṇiyā aṅguttaranikāya-aṭṭhakathāya",
  "atk-s0404a30:3_0": "Dasakanipātassa saṁvaṇṇanā niṭṭhitā.",
  "atk-s0404a30:4_0": " Namo tassa bhagavato arahato sammāsambuddhassa",
  "atk-s0404a30:5_0": "Aṅguttaranikāye",
  "atk-s0404a30:6_0": "Ekādasakanipāta-aṭṭhakathā"
}