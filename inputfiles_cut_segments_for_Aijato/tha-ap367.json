{
  "tha-ap367:0.1_0": "Therāpadāna",
  "tha-ap367:0.2_0": "Mandāravapupphiyavagga",
  "tha-ap367:0.3_0": "5. Aṅkolapupphiyattheraapadāna",
  "tha-ap367:1.1_0": "“Padumo nāma sambuddho,",
  "tha-ap367:1.2_0": "cittakūṭe vasī tadā;",
  "tha-ap367:1.3_0": "Disvāna taṁ ahaṁ buddhaṁ,",
  "tha-ap367:1.4_0": "sayambhuṁ aparājitaṁ.",
  "tha-ap367:2.1_0": "Aṅkolaṁ pupphitaṁ disvā,",
  "tha-ap367:2.2_0": "ocinitvānahaṁ tadā;",
  "tha-ap367:2.3_0": "Upagantvāna sambuddhaṁ,",
  "tha-ap367:2.4_0": "pūjayiṁ padumaṁ jinaṁ.",
  "tha-ap367:3.1_0": "Ekattiṁse ito kappe,",
  "tha-ap367:3.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap367:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap367:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap367:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap367:4.2_0": "…pe…",
  "tha-ap367:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap367:5.1_0": "Itthaṁ sudaṁ āyasmā aṅkolapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap367:6.1_0": "Aṅkolapupphiyattherassāpadānaṁ pañcamaṁ."
}