{
  "pli-tv-pvr6:0.1_0": "Parivāra",
  "pli-tv-pvr6:0.2_0": "6. Khandhakapucchāvāra",
  "pli-tv-pvr6:1.1_0": "Upasampadaṁ pucchissaṁ sanidānaṁ saniddesaṁ,",
  "pli-tv-pvr6:1.2_0": "Samukkaṭṭhapadānaṁ kati āpattiyo;",
  "pli-tv-pvr6:1.3_0": "Upasampadaṁ vissajjissaṁ sanidānaṁ saniddesaṁ,",
  "pli-tv-pvr6:1.4_0": "Samukkaṭṭhapadānaṁ dve āpattiyo. ",
  "pli-tv-pvr6:2.1_0": "Uposathaṁ pucchissaṁ sanidānaṁ saniddesaṁ,",
  "pli-tv-pvr6:2.2_0": "Samukkaṭṭhapadānaṁ kati āpattiyo;",
  "pli-tv-pvr6:2.3_0": "Uposathaṁ vissajjissaṁ sanidānaṁ saniddesaṁ,",
  "pli-tv-pvr6:2.4_0": "Samukkaṭṭhapadānaṁ tisso āpattiyo. ",
  "pli-tv-pvr6:3.1_0": "Vassūpanāyikaṁ pucchissaṁ sanidānaṁ saniddesaṁ,",
  "pli-tv-pvr6:3.2_0": "Samukkaṭṭhapadānaṁ kati āpattiyo;",
  "pli-tv-pvr6:3.3_0": "Vassūpanāyikaṁ vissajjissaṁ sanidānaṁ saniddesaṁ,",
  "pli-tv-pvr6:3.4_0": "Samukkaṭṭhapadānaṁ ekā āpatti. ",
  "pli-tv-pvr6:4.1_0": "Pavāraṇaṁ pucchissaṁ sanidānaṁ saniddesaṁ,",
  "pli-tv-pvr6:4.2_0": "Samukkaṭṭhapadānaṁ kati āpattiyo;",
  "pli-tv-pvr6:4.3_0": "Pavāraṇaṁ vissajjissaṁ sanidānaṁ saniddesaṁ,",
  "pli-tv-pvr6:4.4_0": "Samukkaṭṭhapadānaṁ tisso āpattiyo. ",
  "pli-tv-pvr6:5.1_0": "Cammasaññuttaṁ pucchissaṁ sanidānaṁ saniddesaṁ,",
  "pli-tv-pvr6:5.2_0": "Samukkaṭṭhapadānaṁ kati āpattiyo;",
  "pli-tv-pvr6:5.3_0": "Cammasaññuttaṁ vissajjissaṁ sanidānaṁ saniddesaṁ,",
  "pli-tv-pvr6:5.4_0": "Samukkaṭṭhapadānaṁ tisso āpattiyo. ",
  "pli-tv-pvr6:6.1_0": "Bhesajjaṁ pucchissaṁ sanidānaṁ saniddesaṁ,",
  "pli-tv-pvr6:6.2_0": "Samukkaṭṭhapadānaṁ kati āpattiyo;",
  "pli-tv-pvr6:6.3_0": "Bhesajjaṁ vissajjissaṁ sanidānaṁ saniddesaṁ,",
  "pli-tv-pvr6:6.4_0": "Samukkaṭṭhapadānaṁ tisso āpattiyo. ",
  "pli-tv-pvr6:7.1_0": "Kathinakaṁ pucchissaṁ sanidānaṁ saniddesaṁ,",
  "pli-tv-pvr6:7.2_0": "Samukkaṭṭhapadānaṁ kati āpattiyo;",
  "pli-tv-pvr6:7.3_0": "Kathinakaṁ vissajjissaṁ sanidānaṁ saniddesaṁ,",
  "pli-tv-pvr6:7.4_0": "Samukkaṭṭhapadānaṁ natthi tattha āpatti. ",
  "pli-tv-pvr6:8.1_0": "Cīvarasaññuttaṁ pucchissaṁ sanidānaṁ saniddesaṁ,",
  "pli-tv-pvr6:8.2_0": "Samukkaṭṭhapadānaṁ kati āpattiyo;",
  "pli-tv-pvr6:8.3_0": "Cīvarasaññuttaṁ vissajjissaṁ sanidānaṁ saniddesaṁ,",
  "pli-tv-pvr6:8.4_0": "Samukkaṭṭhapadānaṁ tisso āpattiyo. ",
  "pli-tv-pvr6:9.1_0": "Campeyyakaṁ pucchissaṁ sanidānaṁ saniddesaṁ,",
  "pli-tv-pvr6:9.2_0": "Samukkaṭṭhapadānaṁ kati āpattiyo;",
  "pli-tv-pvr6:9.3_0": "Campeyyakaṁ vissajjissaṁ sanidānaṁ saniddesaṁ,",
  "pli-tv-pvr6:9.4_0": "Samukkaṭṭhapadānaṁ ekā āpatti. ",
  "pli-tv-pvr6:10.1_0": "Kosambakaṁ pucchissaṁ sanidānaṁ saniddesaṁ,",
