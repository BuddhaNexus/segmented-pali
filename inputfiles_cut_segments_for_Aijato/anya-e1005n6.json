{
  "anya-e1005n6:0_0": "7. Pakiṇṇakakaṇḍo",
  "anya-e1005n6:1_0": "138.",
  "anya-e1005n6:2_0": " Itthimisse kutosīlaṁ,",
  "anya-e1005n6:3_0": "Maṁsa bhakkhe kutodayā;",
  "anya-e1005n6:4_0": "Surā pāne kutosaccaṁ,",
  "anya-e1005n6:5_0": "Mahālobhe kutohirī;",
  "anya-e1005n6:6_0": "Mahātande kutosippaṁ,",
  "anya-e1005n6:7_0": "Mahā kodhe kutodhanaṁ.",
  "anya-e1005n6:8_0": "139.",
  "anya-e1005n6:9_0": " Surā yogo vikālo ca,",
  "anya-e1005n6:10_0": "Samajja caraṇālasaṁ;",
  "anya-e1005n6:11_0": "Khiḍḍādhutto pāpamitto,",
  "anya-e1005n6:12_0": "Bhoganāsamukhā ime.",
  "anya-e1005n6:13_0": "140.",
  "anya-e1005n6:14_0": " Divā nādikkhā vattabbaṁ,",
  "anya-e1005n6:15_0": "Ratto nāvacanena ca;",
  "anya-e1005n6:16_0": "Sañcareyya bhayā bhīto,",
  "anya-e1005n6:17_0": "Vane vanacarī yathā.",
  "anya-e1005n6:18_0": "141.",
  "anya-e1005n6:19_0": " Jīvantāpi matāpañca,",
  "anya-e1005n6:20_0": "Byāsena parikittitā;",
  "anya-e1005n6:21_0": "Dukkhito byādhitomūḷho,",
  "anya-e1005n6:22_0": "Iṇavā nityasevako.",
  "anya-e1005n6:23_0": "142.",
  "anya-e1005n6:24_0": " Anāgataṁ bhayaṁ disvā,",
  "anya-e1005n6:25_0": "Dūrato parivajjaye;",
  "anya-e1005n6:26_0": "Āgatañca bhayaṁ disvā,",
  "anya-e1005n6:27_0": "A bhīto hoti paṇḍito.",
  "anya-e1005n6:28_0": "143.",
  "anya-e1005n6:29_0": " Niddāluko pamattoca,",
  "anya-e1005n6:30_0": "Sukhatto rogavālaso;",
  "anya-e1005n6:31_0": "Mahiccho kammārāmoca,",
  "anya-e1005n6:32_0": "Satte te satthavajjitā.",
  "anya-e1005n6:33_0": "144.",
  "anya-e1005n6:34_0": " Duggataṁ gaccha he lābha,",
  "anya-e1005n6:35_0": "Lābhī lābhena pūrati;",
  "anya-e1005n6:36_0": "Thale pavassa pajjunna,",
  "anya-e1005n6:37_0": "Sindhu āpena pūrati;",
  "anya-e1005n6:38_0": "Natthidaṁ kammappadhānakaṁ.",
  "anya-e1005n6:39_0": "145.",
  "anya-e1005n6:40_0": " Na hi koci kate kicce,",
  "anya-e1005n6:41_0": "Kattāraṁ samupekkhate;",
  "anya-e1005n6:42_0": "Tasmā sabbāni kiccāni,",
  "anya-e1005n6:43_0": "Sāva sesena kāraye.",
  "anya-e1005n6:44_0": "146.",
  "anya-e1005n6:45_0": " Tūlaṁ sallahukaṁ loke,",
  "anya-e1005n6:46_0": "Tato cāpalla-jātiko;",
  "anya-e1005n6:47_0": "Tato vuḍḍha manovādo,",
  "anya-e1005n6:48_0": "Pamatto buddhasāsane.",
  "anya-e1005n6:49_0": "147.",
  "anya-e1005n6:50_0": " Pāsāṇachattaṁ garukaṁ,",
  "anya-e1005n6:51_0": "Tato devānacikkhaṇaṁ;",
  "anya-e1005n6:52_0": "Tato vuḍḍhānamovādo,",
  "anya-e1005n6:53_0": "Tato buddhassa sāsanaṁ.",
  "anya-e1005n6:54_0": "148.",
  "anya-e1005n6:55_0": " Kāyassa dakkhiṇa hattho,",
  "anya-e1005n6:56_0": "Doso ettha kaniṭṭhako;",
  "anya-e1005n6:57_0": "Kaṇṇa ghānāna-makkhīnaṁ,",
  "anya-e1005n6:58_0": "Vāmo tu pāda-pāsako.",
  "anya-e1005n6:59_0": "149.",
  "anya-e1005n6:60_0": " Tambūlassa majjha patte,",
  "anya-e1005n6:61_0": "Kuvero rakkhatī sadā;",
  "anya-e1005n6:62_0": "Mūlamhi rakkhati yakkho,",
  "anya-e1005n6:63_0": "Aggamhi kālakaṇṇikā;",
  "anya-e1005n6:64_0": "Tāni bhuñjeyya chinditvā,",
  "anya-e1005n6:65_0": "Sirī evaṁ pavaḍḍhati.",
  "anya-e1005n6:66_0": "150.",
  "anya-e1005n6:67_0": " Sampuṇṇarakkho brahmāva,",
  "anya-e1005n6:68_0": "Accurakkho ca bissaṇo;",
  "anya-e1005n6:69_0": "Tasmā hi te pūjayantu,",
  "anya-e1005n6:70_0": "Sadā mānenti taṁ naraṁ.",
  "anya-e1005n6:71_0": "151.",
  "anya-e1005n6:72_0": " Goṇā hi sabbagihīnaṁ,",
  "anya-e1005n6:73_0": "Posakā bhogadāyakā;",
  "anya-e1005n6:74_0": "Tasmā hi mātā pitūva,",
  "anya-e1005n6:75_0": "Mānaye sakkareyya ca.",
  "anya-e1005n6:76_0": "152.",
  "anya-e1005n6:77_0": " Yeca khādanti gomaṁsaṁ,",
  "anya-e1005n6:78_0": "Mātu maṁsaṁva khādare;",
  "anya-e1005n6:79_0": "Matesu tesu gijjhānaṁ,",
  "anya-e1005n6:80_0": "Dade sote ca vāhaye.",
  "anya-e1005n6:81_0": "153.",
  "anya-e1005n6:82_0": " Gurusiddho sippārambho,",
  "anya-e1005n6:83_0": "Ravi sokrā ca majjhimo;",
  "anya-e1005n6:84_0": "Na sippo buddhacandaro,",
  "anya-e1005n6:85_0": "Sorī aṅgāca maraṇaṁ.",
  "anya-e1005n6:86_0": "154.",
  "anya-e1005n6:87_0": " Aṭṭhamiyaṁ guruṁ hanti,",
  "anya-e1005n6:88_0": "Sissaṁ hanti catuddasiṁ;",
  "anya-e1005n6:89_0": "Sippaṁ hanti dasa sippaṁ,",
  "anya-e1005n6:90_0": "Mātāpitā ca puṇṇamiṁ.",
  "anya-e1005n6:91_0": "155.",
  "anya-e1005n6:92_0": " Nāḷikaṁ satta nabhuñje,",
  "anya-e1005n6:93_0": "Na lābuṁ navamaṁ tathā;",
  "anya-e1005n6:94_0": "Dvādasa prinnaṁtriminaṁ,",
  "anya-e1005n6:95_0": "Bhuñje sippaṁ vinassati.",
  "anya-e1005n6:96_0": "156.",
  "anya-e1005n6:97_0": " Ekaṁ caje kulaatthaṁ,",
  "anya-e1005n6:98_0": "Gāmassatthaṁ kuvaṁ caje;",
  "anya-e1005n6:99_0": "Gāma caje janapadatthaṁ,",
  "anya-e1005n6:100_0": "Attatthaṁ pathaviṁ caje.",
  "anya-e1005n6:101_0": "157.",
  "anya-e1005n6:102_0": " Desaṁ ossajja gacchanti,",
  "anya-e1005n6:103_0": "Sīho sappuriso gajo;",
  "anya-e1005n6:104_0": "Tattheva nidhanaṁ yanti,",
  "anya-e1005n6:105_0": "Kāko kāpuriso migo.",
  "anya-e1005n6:106_0": "158.",
  "anya-e1005n6:107_0": " Yamhi padese na māno,",
  "anya-e1005n6:108_0": "Na pemaṁ na ca bandhavā;",
  "anya-e1005n6:109_0": "Na ca vijjāgāho koci,",
  "anya-e1005n6:110_0": "Na tattha vasanaṁ kare.",
  "anya-e1005n6:111_0": "159.",
  "anya-e1005n6:112_0": " Caratyekena pādena,",
  "anya-e1005n6:113_0": "Tiṭṭhatyekena paṇḍito;",
  "anya-e1005n6:114_0": "A nisamma paraṁ ṭhānaṁ,",
  "anya-e1005n6:115_0": "Na pubbamālayaṁ jahe.",
  "anya-e1005n6:116_0": "160.",
  "anya-e1005n6:117_0": " Dhana dhañña payogesu,",
  "anya-e1005n6:118_0": "Tathā vijjāgamesu ca;",
  "anya-e1005n6:119_0": "Dūtesu apacāresu,",
  "anya-e1005n6:120_0": "Cajjā lajjā tadā bhave.",
  "anya-e1005n6:121_0": "161.",
  "anya-e1005n6:122_0": " Dvi guṇo thīnamāhāro,",
  "anya-e1005n6:123_0": "Buddhicāpi catugguṇo;",
  "anya-e1005n6:124_0": "Chagguṇo hoti vāyāmo,",
  "anya-e1005n6:125_0": "Kāmotvaṭṭha-guṇo bhave.",
  "anya-e1005n6:126_0": "162.",
  "anya-e1005n6:127_0": " Pabbe pabbe kamenucchu,",
  "anya-e1005n6:128_0": "Visesarasavāggato;",
  "anya-e1005n6:129_0": "Tathā sumettiko sādhu,",
  "anya-e1005n6:130_0": "Viparītova dujjano.",
  "anya-e1005n6:131_0": "163.",
  "anya-e1005n6:132_0": " Kassako vāṇijo macco,",
  "anya-e1005n6:133_0": "Samaṇo suta sīlavā;",
  "anya-e1005n6:134_0": "Tesu vipula jātesu,",
  "anya-e1005n6:135_0": "Raṭṭhampi vipulaṁ siyā.",
  "anya-e1005n6:136_0": "164.",
  "anya-e1005n6:137_0": " Asajjhāya malā mantā,",
  "anya-e1005n6:138_0": "Anuṭṭhāna malā gharā;",
  "anya-e1005n6:139_0": "Malaṁ vaṇṇassa kosajjaṁ,",
  "anya-e1005n6:140_0": "Pamādo rakkhato malaṁ.",
  "anya-e1005n6:141_0": "165.",
  "anya-e1005n6:142_0": " Hīnānaṁ gacchate vittaṁ,",
  "anya-e1005n6:143_0": "Vīrānaṁ santakattanaṁ;",
  "anya-e1005n6:144_0": "Vadanti ca hīnā janā,",
  "anya-e1005n6:145_0": "Pubba-kammappadhānakā.",
  "anya-e1005n6:146_0": "166.",
  "anya-e1005n6:147_0": " Na vadanti cevaṁdhīrā,",
  "anya-e1005n6:148_0": "Vāyamiṁsu sabbakamme;",
  "anya-e1005n6:149_0": "Na ce sijjhati taṁ kammaṁ,",
  "anya-e1005n6:150_0": "A-phalaṁ eva ko doso.",
  "anya-e1005n6:151_0": "167.",
  "anya-e1005n6:152_0": " Nīcaṁ kulaṁ nipaññaṁ vā,",
  "anya-e1005n6:153_0": "Nirūpaṁ nibalaṁ samaṁ;",
  "anya-e1005n6:154_0": "Imaṁ kālaṁ chuttakālaṁ,",
  "anya-e1005n6:155_0": "Dhanameva visesakaṁ.",
  "anya-e1005n6:156_0": "Pakiṇṇakakaṇḍo niṭṭhito.",
  "anya-e1005n6:157_0": "Paṇḍito sujano kaṇḍo,",
  "anya-e1005n6:158_0": "Dujjano mitta-itthi ca;",
  "anya-e1005n6:159_0": "Rājā pakiṇṇako cāti,",
  "anya-e1005n6:160_0": "Satta-kaṇḍa-vibhūsitaṁ.",
  "anya-e1005n6:161_0": "Visuddhā cāra-therena,",
  "anya-e1005n6:162_0": "Visuddhārāma-vāsinā;",
  "anya-e1005n6:163_0": "Sabba-kulānamatthāya,",
  "anya-e1005n6:164_0": "Visodhitaṁ pathakkhaye."
}