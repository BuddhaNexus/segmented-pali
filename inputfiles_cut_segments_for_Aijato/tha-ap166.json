{
  "tha-ap166:0.1_0": "Therāpadāna",
  "tha-ap166:0.2_0": "Supāricariyavagga",
  "tha-ap166:0.3_0": "4. Desapūjakattheraapadāna",
  "tha-ap166:1.1_0": "“Atthadassī tu bhagavā,",
  "tha-ap166:1.2_0": "lokajeṭṭho narāsabho;",
  "tha-ap166:1.3_0": "Abbhuggantvāna vehāsaṁ,",
  "tha-ap166:1.4_0": "gacchate anilañjase.",
  "tha-ap166:2.1_0": "Yamhi dese ṭhito satthā,",
  "tha-ap166:2.2_0": "abbhuggacchi mahāmuni;",
  "tha-ap166:2.3_0": "Tāhaṁ desaṁ apūjesiṁ,",
  "tha-ap166:2.4_0": "pasanno sehi pāṇibhi.",
  "tha-ap166:3.1_0": "Aṭṭhārase kappasate,",
  "tha-ap166:3.2_0": "addasaṁ yaṁ mahāmuniṁ;",
  "tha-ap166:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap166:3.4_0": "desapūjāyidaṁ phalaṁ.",
  "tha-ap166:4.1_0": "Ekādase kappasate,",
  "tha-ap166:4.2_0": "gosujātasanāmako;",
  "tha-ap166:4.3_0": "Sattaratanasampanno,",
  "tha-ap166:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap166:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap166:5.2_0": "…pe…",
  "tha-ap166:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap166:6.1_0": "Itthaṁ sudaṁ āyasmā desapūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap166:7.1_0": "Desapūjakattherassāpadānaṁ catutthaṁ."
}