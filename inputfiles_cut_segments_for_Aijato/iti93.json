{
  "iti93:0.1_0": "Itivuttaka 93",
  "iti93:0.2_0": "Tikanipāta",
  "iti93:0.3_0": "Pañcamavagga",
  "iti93:0.4_0": "Aggisutta",
  "iti93:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti93:2.1_0": "“Tayome, bhikkhave, aggī.",
  "iti93:2.2_0": "Katame tayo?",
  "iti93:2.3_0": "Rāgaggi, dosaggi, mohaggi—",
  "iti93:2.4_0": "ime kho, bhikkhave, tayo aggī”ti.",
  "iti93:2.5_0": "Etamatthaṁ bhagavā avoca.",
  "iti93:2.6_0": "Tatthetaṁ iti vuccati:",
  "iti93:3.1_0": "“Rāgaggi dahati macce,",
  "iti93:3.2_0": "ratte kāmesu mucchite;",
  "iti93:3.3_0": "Dosaggi pana byāpanne,",
  "iti93:3.4_0": "nare pāṇātipātino.",
  "iti93:4.1_0": "Mohaggi pana sammūḷhe,",
  "iti93:4.2_0": "ariyadhamme akovide;",
  "iti93:4.3_0": "Ete aggī ajānantā,",
  "iti93:4.4_0": "sakkāyābhiratā pajā.",
  "iti93:5.1_0": "Te vaḍḍhayanti nirayaṁ,",
  "iti93:5.2_0": "tiracchānañca yoniyo;",
  "iti93:5.3_0": "Asuraṁ pettivisayaṁ,",
  "iti93:5.4_0": "amuttā mārabandhanā.",
  "iti93:6.1_0": "Ye ca rattindivā yuttā,",
  "iti93:6.2_0": "sammāsambuddhasāsane;",
  "iti93:6.3_0": "Te nibbāpenti rāgaggiṁ,",
  "iti93:6.4_0": "niccaṁ asubhasaññino.",
  "iti93:7.1_0": "Dosaggiṁ pana mettāya,",
  "iti93:7.2_0": "nibbāpenti naruttamā;",
  "iti93:7.3_0": "Mohaggiṁ pana paññāya,",
  "iti93:7.4_0": "yāyaṁ nibbedhagāminī.",
  "iti93:8.1_0": "Te nibbāpetvā nipakā,",
  "iti93:8.2_0": "rattindivamatanditā;",
  "iti93:8.3_0": "Asesaṁ parinibbanti,",
  "iti93:8.4_0": "asesaṁ dukkhamaccaguṁ.",
  "iti93:9.1_0": "Ariyaddasā vedaguno,",
  "iti93:9.2_0": "sammadaññāya paṇḍitā;",
  "iti93:9.3_0": "Jātikkhayamabhiññāya,",
  "iti93:9.4_0": "nāgacchanti punabbhavan”ti.",
  "iti93:10.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti93:10.2_0": "Catutthaṁ."
}