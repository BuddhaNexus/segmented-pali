{
  "kv6.4:0.1_0": "Kathāvatthu",
  "kv6.4:0.2_0": "Mahāpaṇṇāsaka",
  "kv6.4:0.3_0": "Chaṭṭhavagga",
  "kv6.4:0.4_0": "Āruppakathā",
  "kv6.4:1.1_0": "Ākāsānañcāyatanaṁ asaṅkhatanti?",
  "kv6.4:1.2_0": "Āmantā.",
  "kv6.4:1.3_0": "Nibbānaṁ tāṇaṁ leṇaṁ saraṇaṁ parāyanaṁ accutaṁ amatanti?",
  "kv6.4:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv6.4:1.5_0": "ākāsānañcāyatanaṁ asaṅkhataṁ, nibbānaṁ asaṅkhatanti?",
  "kv6.4:1.6_0": "Āmantā.",
  "kv6.4:1.7_0": "Dve asaṅkhatānīti?",
  "kv6.4:1.8_0": "Na hevaṁ vattabbe …pe…",
  "kv6.4:1.9_0": "dve asaṅkhatānīti?",
  "kv6.4:1.10_0": "Āmantā.",
  "kv6.4:1.11_0": "Dve tāṇāni …pe…",
  "kv6.4:1.12_0": "antarikā vāti?",
  "kv6.4:1.13_0": "Na hevaṁ vattabbe …pe….",
  "kv6.4:2.1_0": "Ākāsānañcāyatanaṁ asaṅkhatanti?",
  "kv6.4:2.2_0": "Āmantā.",
  "kv6.4:2.3_0": "Ākāsānañcāyatanaṁ bhavo gati sattāvāso saṁsāro yoni viññāṇaṭṭhiti attabhāvapaṭilābhoti?",
  "kv6.4:2.4_0": "Āmantā.",
  "kv6.4:2.5_0": "Asaṅkhataṁ bhavo gati sattāvāso saṁsāro yoni viññāṇaṭṭhiti attabhāvapaṭilābhoti?",
  "kv6.4:2.6_0": "Na hevaṁ vattabbe …pe….",
  "kv6.4:3.1_0": "Atthi ākāsānañcāyatanūpagaṁ kammanti?",
  "kv6.4:3.2_0": "Āmantā.",
  "kv6.4:3.3_0": "Atthi asaṅkhatūpagaṁ kammanti?",
  "kv6.4:3.4_0": "Na hevaṁ vattabbe …pe…",
  "kv6.4:3.5_0": "atthi ākāsānañcāyatanūpagā sattāti?",
  "kv6.4:3.6_0": "Āmantā.",
  "kv6.4:3.7_0": "Atthi asaṅkhatūpagā sattāti?",
  "kv6.4:3.8_0": "Na hevaṁ vattabbe …pe….",
  "kv6.4:4.1_0": "Ākāsānañcāyatane sattā jāyanti jīyanti mīyanti cavanti upapajjantīti?",
  "kv6.4:4.2_0": "Āmantā.",
  "kv6.4:4.3_0": "Asaṅkhate sattā jāyanti jīyanti mīyanti cavanti upapajjantīti?",
  "kv6.4:4.4_0": "Na hevaṁ vattabbe …pe…",
  "kv6.4:4.5_0": "ākāsānañcāyatane atthi vedanā saññā saṅkhārā viññāṇanti?",
  "kv6.4:4.6_0": "Āmantā.",
  "kv6.4:4.7_0": "Asaṅkhate atthi vedanā saññā saṅkhārā viññāṇanti?",
  "kv6.4:4.8_0": "Na hevaṁ vattabbe …pe…",
  "kv6.4:4.9_0": "ākāsānañcāyatanaṁ catuvokārabhavoti?",
  "kv6.4:4.10_0": "Āmantā.",
  "kv6.4:4.11_0": "Asaṅkhataṁ catuvokārabhavoti?",
  "kv6.4:4.12_0": "Na hevaṁ vattabbe …pe….",
  "kv6.4:5.1_0": "Na vattabbaṁ—",
  "kv6.4:5.2_0": "“cattāro āruppā asaṅkhatā”ti?",
  "kv6.4:5.3_0": "Āmantā.",
  "kv6.4:5.4_0": "Nanu cattāro āruppā anejā vuttā bhagavatāti?",
  "kv6.4:5.5_0": "Āmantā.",
  "kv6.4:5.6_0": "Hañci cattāro āruppā anejā vuttā bhagavatā, tena vata re vattabbe—",
  "kv6.4:5.7_0": "“cattāro āruppā asaṅkhatā”ti.",
  "kv6.4:5.8_0": "Āruppakathā niṭṭhitā."
}