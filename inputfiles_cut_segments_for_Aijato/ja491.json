{
  "ja491:0.1_0": "Jātaka",
  "ja491:0.2_0": "Pakiṇṇakanipāta",
  "ja491:0.3_0": "Sālikedāravagga",
  "ja491:0.4_0": "8. Mahāmorajātaka",
  "ja491:1.1_0": "“Sace hi tyāhaṁ dhanahetu gāhito,",
  "ja491:1.2_0": "Mā maṁ vadhī jīvagāhaṁ gahetvā;",
  "ja491:1.3_0": "Rañño ca maṁ samma upantikaṁ nehi,",
  "ja491:1.4_0": "Maññe dhanaṁ lacchasinapparūpaṁ”.",
  "ja491:2.1_0": "“Na me ayaṁ tuyha vadhāya ajja,",
  "ja491:2.2_0": "Samāhito cāpadhure khurappo;",
  "ja491:2.3_0": "Pāsañca tyāhaṁ adhipātayissaṁ,",
  "ja491:2.4_0": "Yathāsukhaṁ gacchatu morarājā”.",
  "ja491:3.1_0": "“Yaṁ satta vassāni mamānubandhi,",
  "ja491:3.2_0": "Rattindivaṁ khuppipāsaṁ sahanto;",
  "ja491:3.3_0": "Atha kissa maṁ pāsavasūpanītaṁ,",
  "ja491:3.4_0": "Pamuttave icchasi bandhanasmā.",
  "ja491:4.1_0": "Pāṇātipātā virato nusajja,",
  "ja491:4.2_0": "Abhayaṁ nu te sabbabhūtesu dinnaṁ;",
  "ja491:4.3_0": "Yaṁ maṁ tuvaṁ pāsavasūpanītaṁ,",
  "ja491:4.4_0": "Pamuttave icchasi bandhanasmā”.",
  "ja491:5.1_0": "“Pāṇātipātā viratassa brūhi,",
  "ja491:5.2_0": "Abhayañca yo sabbabhūtesu deti;",
  "ja491:5.3_0": "Pucchāmi taṁ morarājetamatthaṁ,",
  "ja491:5.4_0": "Ito cuto kiṁ labhate sukhaṁ so”.",
  "ja491:6.1_0": "“Pāṇātipātā viratassa brūmi,",
  "ja491:6.2_0": "Abhayañca yo sabbabhūtesu deti;",
  "ja491:6.3_0": "Diṭṭheva dhamme labhate pasaṁsaṁ,",
  "ja491:6.4_0": "Saggañca so yāti sarīrabhedā”.",
  "ja491:7.1_0": "“Na santi devā iti āhu eke,",
  "ja491:7.2_0": "Idheva jīvo vibhavaṁ upeti;",
  "ja491:7.3_0": "Tathā phalaṁ sukatadukkaṭānaṁ,",
  "ja491:7.4_0": "Dattupaññattañca vadanti dānaṁ;",
  "ja491:7.5_0": "Tesaṁ vaco arahataṁ saddahāno,",
  "ja491:7.6_0": "Tasmā ahaṁ sakuṇe bādhayāmi”.",
  "ja491:8.1_0": "“Cando ca suriyo ca ubho sudassanā,",
  "ja491:8.2_0": "Gacchanti obhāsayamantalikkhe;",
  "ja491:8.3_0": "Imassa lokassa parassa vā te,",
  "ja491:8.4_0": "Kathaṁ nu te āhu manussaloke”.",
  "ja491:9.1_0": "“Cando ca suriyo ca ubho sudassanā,",
  "ja491:9.2_0": "Gacchanti obhāsayamantalikkhe;",
  "ja491:9.3_0": "Parassa lokassa na te imassa,",
  "ja491:9.4_0": "Devāti te āhu manussaloke”.",
  "ja491:10.1_0": "“Ettheva te nīhatā hīnavādā,",
  "ja491:10.2_0": "Ahetukā ye na vadanti kammaṁ;",
  "ja491:10.3_0": "Tathā phalaṁ sukatadukkaṭānaṁ,",
  "ja491:10.4_0": "Dattupaññattaṁ ye ca vadanti dānaṁ”.",
  "ja491:11.1_0": "“Addhā hi saccaṁ vacanaṁ tavedaṁ,",
  "ja491:11.2_0": "Kathañhi dānaṁ aphalaṁ bhaveyya;",
  "ja491:11.3_0": "Tathā phalaṁ sukatadukkaṭānaṁ,",
  "ja491:11.4_0": "Dattupaññattañca kathaṁ bhaveyya.",
  "ja491:12.1_0": "Kathaṅkaro kintikaro kimācaraṁ,",
  "ja491:12.2_0": "Kiṁ sevamāno kena tapoguṇena;",
  "ja491:12.3_0": "Akkhāhi me morarājetamatthaṁ,",
  "ja491:12.4_0": "Yathā ahaṁ no nirayaṁ pateyyaṁ”.",
  "ja491:13.1_0": "“Ye keci atthi samaṇā pathabyā,",
  "ja491:13.2_0": "Kāsāyavatthā anagāriyā te;",
  "ja491:13.3_0": "Pātova piṇḍāya caranti kāle,",
  "ja491:13.4_0": "Vikālacariyā viratā hi santo.",
  "ja491:14.1_0": "Te tattha kālenupasaṅkamitvā,",
  "ja491:14.2_0": "Pucchāhi yaṁ te manaso piyaṁ siyā;",
  "ja491:14.3_0": "Te te pavakkhanti yathāpajānaṁ,",
  "ja491:14.4_0": "Imassa lokassa parassa catthaṁ”.",
  "ja491:15.1_0": "“Tacaṁva jiṇṇaṁ urago purāṇaṁ,",
  "ja491:15.2_0": "Paṇḍūpalāsaṁ harito dumova;",
  "ja491:15.3_0": "Esappahīno mama luddabhāvo,",
  "ja491:15.4_0": "Jahāmahaṁ luddakabhāvamajja.",
  "ja491:16.1_0": "Ye cāpi me sakuṇā atthi baddhā,",
  "ja491:16.2_0": "Satāninekāni nivesanasmiṁ;",
  "ja491:16.3_0": "Tesampahaṁ jīvitamajja dammi,",
  "ja491:16.4_0": "Mokkhañca te pattā sakaṁ niketaṁ”.",
  "ja491:17.1_0": "“Luddo carī pāsahattho araññe,",
  "ja491:17.2_0": "Bādhetu morādhipatiṁ yasassiṁ;",
  "ja491:17.3_0": "Bandhitvā morādhipatiṁ yasassiṁ,",
  "ja491:17.4_0": "Dukkhā sa pamucci yathāhaṁ pamutto”ti.",
  "ja491:18.1_0": "Mahāmorajātakaṁ aṭṭhamaṁ."
}