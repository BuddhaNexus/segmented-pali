{
  "tha-ap201:0.1_0": "Therāpadāna",
  "tha-ap201:0.2_0": "Tamālapupphiyavagga",
  "tha-ap201:0.3_0": "9 Kiṅkaṇikapupphiyattheraapadāna",
  "tha-ap201:1.1_0": "“Sumaṅgaloti nāmena,",
  "tha-ap201:1.2_0": "sayambhū aparājito;",
  "tha-ap201:1.3_0": "Pavanā nikkhamitvāna,",
  "tha-ap201:1.4_0": "nagaraṁ pāvisī jino.",
  "tha-ap201:2.1_0": "Piṇḍacāraṁ caritvāna,",
  "tha-ap201:2.2_0": "nikkhamma nagarā muni;",
  "tha-ap201:2.3_0": "Katakiccova sambuddho,",
  "tha-ap201:2.4_0": "so vasī vanamantare.",
  "tha-ap201:3.1_0": "Kiṅkaṇipupphaṁ paggayha,",
  "tha-ap201:3.2_0": "buddhassa abhiropayiṁ;",
  "tha-ap201:3.3_0": "Pasannacitto sumano,",
  "tha-ap201:3.4_0": "sayambhussa mahesino.",
  "tha-ap201:4.1_0": "Catunnavutito kappe,",
  "tha-ap201:4.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap201:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap201:4.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap201:5.1_0": "Chaḷāsītimhito kappe,",
  "tha-ap201:5.2_0": "apilāsisanāmako;",
  "tha-ap201:5.3_0": "Sattaratanasampanno,",
  "tha-ap201:5.4_0": "cakkavattī mahabbalo.",
  "tha-ap201:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap201:6.2_0": "…pe…",
  "tha-ap201:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap201:7.1_0": "Itthaṁ sudaṁ āyasmā kiṅkaṇikapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap201:8.1_0": "Kiṅkaṇikapupphiyattherassāpadānaṁ navamaṁ."
}