{
  "tha-ap123:0.1_0": "Therāpadāna",
  "tha-ap123:0.2_0": "Sereyyavagga",
  "tha-ap123:0.3_0": "1. Sereyyakattheraapadāna",
  "tha-ap123:1.1_0": "“Ajjhāyako mantadharo,",
  "tha-ap123:1.2_0": "tiṇṇaṁ vedāna pāragū;",
  "tha-ap123:1.3_0": "Abbhokāse ṭhito santo,",
  "tha-ap123:1.4_0": "addasaṁ lokanāyakaṁ.",
  "tha-ap123:2.1_0": "Sīhaṁ yathā vanacaraṁ,",
  "tha-ap123:2.2_0": "byaggharājaṁva nittasaṁ;",
  "tha-ap123:2.3_0": "Tidhāpabhinnamātaṅgaṁ,",
  "tha-ap123:2.4_0": "kuñjaraṁva mahesinaṁ.",
  "tha-ap123:3.1_0": "Sereyyakaṁ gahetvāna,",
  "tha-ap123:3.2_0": "ākāse ukkhipiṁ ahaṁ;",
  "tha-ap123:3.3_0": "Buddhassa ānubhāvena,",
  "tha-ap123:3.4_0": "parivārenti sabbaso.",
  "tha-ap123:4.1_0": "Adhiṭṭhahi mahāvīro,",
  "tha-ap123:4.2_0": "sabbaññū lokanāyako;",
  "tha-ap123:4.3_0": "Samantā pupphacchadanā,",
  "tha-ap123:4.4_0": "okiriṁsu narāsabhaṁ.",
  "tha-ap123:5.1_0": "Tato sā pupphakañcukā,",
  "tha-ap123:5.2_0": "antovaṇṭā bahimukhā;",
  "tha-ap123:5.3_0": "Sattāhaṁ chadanaṁ katvā,",
  "tha-ap123:5.4_0": "tato antaradhāyatha.",
  "tha-ap123:6.1_0": "Tañca acchariyaṁ disvā,",
  "tha-ap123:6.2_0": "abbhutaṁ lomahaṁsanaṁ;",
  "tha-ap123:6.3_0": "Buddhe cittaṁ pasādesiṁ,",
  "tha-ap123:6.4_0": "sugate lokanāyake.",
  "tha-ap123:7.1_0": "Tena cittappasādena,",
  "tha-ap123:7.2_0": "sukkamūlena codito;",
  "tha-ap123:7.3_0": "Kappānaṁ satasahassaṁ,",
  "tha-ap123:7.4_0": "duggatiṁ nupapajjahaṁ.",
  "tha-ap123:8.1_0": "Pannarasasahassamhi,",
  "tha-ap123:8.2_0": "kappānaṁ pañcavīsati;",
  "tha-ap123:8.3_0": "Vītamalā samānā ca,",
  "tha-ap123:8.4_0": "cakkavattī mahabbalā.",
  "tha-ap123:9.1_0": "Paṭisambhidā catasso,",
  "tha-ap123:9.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap123:9.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap123:9.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap123:10.1_0": "Itthaṁ sudaṁ āyasmā sereyyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap123:11.1_0": "Sereyyakattherassāpadānaṁ paṭhamaṁ."
}