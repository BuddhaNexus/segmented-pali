{
  "thag2.4:0.1_0": "Theragāthā",
  "thag2.4:0.2_0": "Dukanipāta",
  "thag2.4:0.3_0": "Paṭhamavagga",
  "thag2.4:0.4_0": "2.4. Gaṅgātīriyattheragāthā",
  "thag2.4:1.1_0": "“Tiṇṇaṁ me tālapattānaṁ,",
  "thag2.4:1.2_0": "gaṅgātīre kuṭī katā;",
  "thag2.4:1.3_0": "Chavasittova me patto,",
  "thag2.4:1.4_0": "paṁsukūlañca cīvaraṁ.",
  "thag2.4:2.1_0": "Dvinnaṁ antaravassānaṁ,",
  "thag2.4:2.2_0": "ekā vācā me bhāsitā;",
  "thag2.4:2.3_0": "Tatiye antaravassamhi,",
  "thag2.4:2.4_0": "tamokhandho padālito”ti.",
  "thag2.4:2.5_0": "… Gaṅgātīriyo thero …."
}