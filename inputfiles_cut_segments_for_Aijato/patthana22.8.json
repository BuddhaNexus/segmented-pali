{
  "patthana22.8:0.1_0": "Paṭṭhānapakaraṇa",
  "patthana22.8:0.2_0": "Dhammapaccanīyānuloma (4), Tikadukapaṭṭhānapāḷi (4)",
  "patthana22.8:0.3_0": "22.8. Kusalattika, Mahantaraduka",
  "patthana22.8:1.1_0": "Nakusalaṁ nasārammaṇaṁ dhammaṁ paṭicca abyākato sārammaṇo dhammo uppajjati hetupaccayā.",
  "patthana22.8:1.1_1": "(Saṅkhittaṁ.)",
  "patthana22.8:2.1_0": "Hetuyā tīṇi …pe… avigate tīṇi."
}