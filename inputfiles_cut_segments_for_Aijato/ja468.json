{
  "ja468:0.1_0": "Jātaka",
  "ja468:0.2_0": "Dvādasakanipāta",
  "ja468:0.3_0": "Cūḷakuṇālavagga",
  "ja468:0.4_0": "5. Janasandhajātaka",
  "ja468:1.1_0": "“Dasa khalu imāni ṭhānāni,",
  "ja468:1.2_0": "Yāni pubbe akaritvā;",
  "ja468:1.3_0": "Sa pacchā manutappati,",
  "ja468:1.4_0": "Iccevāha janasandho.",
  "ja468:2.1_0": "Aladdhā vittaṁ tappati,",
  "ja468:2.2_0": "pubbe asamudānitaṁ;",
  "ja468:2.3_0": "Na pubbe dhanamesissaṁ,",
  "ja468:2.4_0": "iti pacchānutappati.",
  "ja468:3.1_0": "Sakyarūpaṁ pure santaṁ,",
  "ja468:3.2_0": "mayā sippaṁ na sikkhitaṁ;",
  "ja468:3.3_0": "Kicchā vutti asippassa,",
  "ja468:3.4_0": "iti pacchānutappati.",
  "ja468:4.1_0": "Kūṭavedī pure āsiṁ,",
  "ja468:4.2_0": "pisuṇo piṭṭhimaṁsiko;",
  "ja468:4.3_0": "Caṇḍo ca pharuso cāpi,",
  "ja468:4.4_0": "iti pacchānutappati.",
  "ja468:5.1_0": "Pāṇātipātī pure āsiṁ,",
  "ja468:5.2_0": "luddo cāpi anāriyo;",
  "ja468:5.3_0": "Bhūtānaṁ nāpacāyissaṁ,",
  "ja468:5.4_0": "iti pacchānutappati.",
  "ja468:6.1_0": "Bahūsu vata santīsu,",
  "ja468:6.2_0": "anāpādāsu itthisu;",
  "ja468:6.3_0": "Paradāraṁ asevissaṁ,",
  "ja468:6.4_0": "iti pacchānutappati.",
  "ja468:7.1_0": "Bahumhi vata santamhi,",
  "ja468:7.2_0": "annapāne upaṭṭhite;",
  "ja468:7.3_0": "Na pubbe adadaṁ dānaṁ,",
  "ja468:7.4_0": "iti pacchānutappati.",
  "ja468:8.1_0": "Mātaraṁ pitarañcāpi,",
  "ja468:8.2_0": "jiṇṇakaṁ gatayobbanaṁ;",
  "ja468:8.3_0": "Pahu santo na posissaṁ,",
  "ja468:8.4_0": "iti pacchānutappati.",
  "ja468:9.1_0": "Ācariyamanusatthāraṁ,",
  "ja468:9.2_0": "sabbakāmarasāharaṁ;",
  "ja468:9.3_0": "Pitaraṁ atimaññissaṁ,",
  "ja468:9.4_0": "iti pacchānutappati.",
  "ja468:10.1_0": "Samaṇe brāhmaṇe cāpi,",
  "ja468:10.2_0": "sīlavante bahussute;",
  "ja468:10.3_0": "Na pubbe payirupāsissaṁ,",
  "ja468:10.4_0": "iti pacchānutappati.",
  "ja468:11.1_0": "Sādhu hoti tapo ciṇṇo,",
  "ja468:11.2_0": "santo ca payirupāsito;",
  "ja468:11.3_0": "Na ca pubbe tapo ciṇṇo,",
  "ja468:11.4_0": "iti pacchānutappati.",
  "ja468:12.1_0": "Yo ca etāni ṭhānāni,",
  "ja468:12.2_0": "yoniso paṭipajjati;",
  "ja468:12.3_0": "Karaṁ purisakiccāni,",
  "ja468:12.4_0": "sa pacchā nānutappatī”ti.",
  "ja468:13.1_0": "Janasandhajātakaṁ pañcamaṁ."
}