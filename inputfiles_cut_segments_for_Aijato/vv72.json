{
  "vv72:0.1_0": "Vimānavatthu",
  "vv72:0.2_0": "Purisavimāna",
  "vv72:0.3_0": "Pāyāsivagga",
  "vv72:0.4_0": "8. Paṭhamakuṇḍalīvimānavatthu",
  "vv72:1.1_0": "“Alaṅkato malyadharo suvattho,",
  "vv72:1.2_0": "Sukuṇḍalī kappitakesamassu;",
  "vv72:1.3_0": "Āmuttahatthābharaṇo yasassī,",
  "vv72:1.4_0": "Dibbe vimānamhi yathāpi candimā.",
  "vv72:2.1_0": "Dibbā ca vīṇā pavadanti vagguṁ,",
  "vv72:2.2_0": "Aṭṭhaṭṭhakā sikkhitā sādhurūpā;",
  "vv72:2.3_0": "Dibbā ca kaññā tidasacarā uḷārā,",
  "vv72:2.4_0": "Naccanti gāyanti pamodayanti.",
  "vv72:3.1_0": "Deviddhipattosi mahānubhāvo,",
  "vv72:3.2_0": "Manussabhūto kimakāsi puññaṁ;",
  "vv72:3.3_0": "Kenāsi evaṁ jalitānubhāvo,",
  "vv72:3.4_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv72:4.1_0": "So devaputto attamano,",
  "vv72:4.2_0": "…pe…",
  "vv72:4.3_0": "yassa kammassidaṁ phalaṁ.",
  "vv72:5.1_0": "“Ahaṁ manussesu manussabhūto,",
  "vv72:5.2_0": "Disvāna samaṇe sīlavante;",
  "vv72:5.3_0": "Sampannavijjācaraṇe yasassī,",
  "vv72:5.4_0": "Bahussute taṇhakkhayūpapanne;",
  "vv72:5.5_0": "Annañca pānañca pasannacitto,",
  "vv72:5.6_0": "Sakkacca dānaṁ vipulaṁ adāsiṁ.",
  "vv72:6.1_0": "Tena metādiso vaṇṇo,",
  "vv72:6.2_0": "…pe…",
  "vv72:6.3_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv72:7.1_0": "Paṭhamakuṇḍalīvimānaṁ aṭṭhamaṁ."
}