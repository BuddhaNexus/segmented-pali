{
  "iti62:0.1_0": "Itivuttaka 62",
  "iti62:0.2_0": "Tikanipāta",
  "iti62:0.3_0": "Dutiyavagga",
  "iti62:0.4_0": "Indriyasutta",
  "iti62:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti62:2.1_0": "“Tīṇimāni, bhikkhave, indriyāni.",
  "iti62:2.2_0": "Katamāni tīṇi?",
  "iti62:2.3_0": "Anaññātaññassāmītindriyaṁ, aññindriyaṁ, aññātāvindriyaṁ—",
  "iti62:2.4_0": "imāni kho, bhikkhave, tīṇi indriyānī”ti.",
  "iti62:2.5_0": "Etamatthaṁ bhagavā avoca.",
  "iti62:2.6_0": "Tatthetaṁ iti vuccati:",
  "iti62:3.1_0": "“Sekhassa sikkhamānassa,",
  "iti62:3.2_0": "ujumaggānusārino;",
  "iti62:3.3_0": "Khayasmiṁ paṭhamaṁ ñāṇaṁ,",
  "iti62:3.4_0": "tato aññā anantarā.",
  "iti62:4.1_0": "Tato aññāvimuttassa,",
  "iti62:4.2_0": "ñāṇaṁ ve hoti tādino;",
  "iti62:4.3_0": "Akuppā me vimuttīti,",
  "iti62:4.4_0": "bhavasaṁyojanakkhayā.",
  "iti62:5.1_0": "Sa ve indriyasampanno,",
  "iti62:5.2_0": "santo santipade rato;",
  "iti62:5.3_0": "Dhāreti antimaṁ dehaṁ,",
  "iti62:5.4_0": "jetvā māraṁ savāhinin”ti.",
  "iti62:6.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti62:6.2_0": "Tatiyaṁ."
}