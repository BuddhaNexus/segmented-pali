{
  "ja39:0.1_0": "Jātaka",
  "ja39:0.2_0": "Ekakanipāta",
  "ja39:0.3_0": "Kulāvakavagga",
  "ja39:0.4_0": "9. Nandajātaka",
  "ja39:1.1_0": "“Maññe sovaṇṇayo rāsi,",
  "ja39:1.2_0": "soṇṇamālā ca nandako;",
  "ja39:1.3_0": "Yattha dāso āmajāto,",
  "ja39:1.4_0": "ṭhito thullāni gajjatī”ti.",
  "ja39:2.1_0": "Nandajātakaṁ navamaṁ."
}