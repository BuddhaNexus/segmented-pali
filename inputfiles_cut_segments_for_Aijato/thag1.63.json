{
  "thag1.63:0.1_0": "Theragāthā",
  "thag1.63:0.2_0": "Ekakanipāta",
  "thag1.63:0.3_0": "Sattamavagga",
  "thag1.63:0.4_0": "1.63. Pakkhattheragāthā",
  "thag1.63:1.1_0": "“Cutā patanti patitā,",
  "thag1.63:1.2_0": "giddhā ca punarāgatā;",
  "thag1.63:1.3_0": "Kataṁ kiccaṁ rataṁ rammaṁ,",
  "thag1.63:1.4_0": "sukhenanvāgataṁ sukhan”ti.",
  "thag1.63:1.5_0": "… Pakkho thero …."
}