{
  "tika-abh06t68:0_0": "Sattavīsatimo paricchedo",
  "tika-abh06t68:1_0": "5. Paññattivibhāgo",
  "tika-abh06t68:2_0": "27. Pabhedakathā",
  "tika-abh06t68:3_0": "1062.",
  "tika-abh06t68:4_0": "Cittaṁ cetasikaṁ rūpaṁ, nibbānampi ca bhājitaṁ;",
  "tika-abh06t68:5_0": "Tasmā dāni yathāyogaṁ, paññattipi pavuccati.",
  "tika-abh06t68:6_0": "1063.",
  "tika-abh06t68:7_0": "Sā cāyaṁ atthapaññatti-nāmapaññattibhedato;",
  "tika-abh06t68:8_0": "Duvidhā hoti paññatti, atthapaññatti tattha ca.",
  "tika-abh06t68:9_0": "1064.",
  "tika-abh06t68:10_0": "Sattasambhārasaṇṭhāna-saṅghāṭapariṇāmato;",
  "tika-abh06t68:11_0": "Vikappupaṭṭhānākāravohārābhinivesato.",
  "tika-abh06t68:12_0": "1065.",
  "tika-abh06t68:13_0": "Tathā pavattasaṅketasiddhā atthā pakappitā;",
  "tika-abh06t68:14_0": "Paññāpīyanti nāmāti, paññattīti pakittitā.",
  "tika-abh06t68:15_0": "1066.",
  "tika-abh06t68:16_0": "Atthā hi paramatthatthā, paññattatthāti ca dvidhā;",
  "tika-abh06t68:17_0": "Tattha ca paramatthatthā, saccikaṭṭhā salakkhaṇā.",
  "tika-abh06t68:18_0": "1067.",
  "tika-abh06t68:19_0": "Paññattatthā saccikaṭṭhasalakkhaṇasabhāvato;",
  "tika-abh06t68:20_0": "Aññathā gahitā taṁtamupādāya pakappitā.",
  "tika-abh06t68:21_0": "1068.",
  "tika-abh06t68:22_0": "Tasmā upādāpaññatti, atthapaññattināmakā;",
  "tika-abh06t68:23_0": "Paññapetabbanāmāva, paññattatthāva sabbathā.",
  "tika-abh06t68:24_0": "1069.",
  "tika-abh06t68:25_0": "Paramatthā yathāvuttā, cittacetasikādayo;",
  "tika-abh06t68:26_0": "Paññattā itthipurisamañcapīṭhapaṭādayo.",
  "tika-abh06t68:27_0": "1070.",
  "tika-abh06t68:28_0": "Yena vuccati taṁ nāmaṁ, paññapetīti vuccati;",
  "tika-abh06t68:29_0": "Paññattīti ca sā nāmapaññattīti tato matā.",
  "tika-abh06t68:30_0": "1071.",
  "tika-abh06t68:31_0": "Saṅkhā samaññā paññatti, vohāroti ca bhājitā;",
  "tika-abh06t68:32_0": "Catudhā paññapetabbapaññattīti hi vaṇṇitā.",
  "tika-abh06t68:33_0": "1072.",
  "tika-abh06t68:34_0": "Tato nāmaṁ nāmakammaṁ, nāmadheyyaṁ athāparaṁ;",
  "tika-abh06t68:35_0": "Nirutti byañjanamabhilāpoti pana bhājitā.",
  "tika-abh06t68:36_0": "1073.",
  "tika-abh06t68:37_0": "Nāmapaññatti nāmāti, paññatti duvidhā katā;",
  "tika-abh06t68:38_0": "Sabbeva dhammā paññattipathāti pana bhājitā.",
  "tika-abh06t68:39_0": "1074.",
  "tika-abh06t68:40_0": "Paramatthapaññattatthā, duvidhā honti tattha ca;",
  "tika-abh06t68:41_0": "Paññattipathāva honti, paramatthā salakkhaṇā.",
  "tika-abh06t68:42_0": "1075.",
  "tika-abh06t68:43_0": "Paññattatthā paññatti ca, paññapetabbamattato;",
  "tika-abh06t68:44_0": "Paññattipathā ca nāmapaññattipathabhāvato.",
  "tika-abh06t68:45_0": "1076.",
  "tika-abh06t68:46_0": "Nāmampi paññāpetabbameva kiñcāpi kenaci;",
  "tika-abh06t68:47_0": "Nāmamevampetaṁ tattha, paññatticceva vaṇṇitaṁ.",
  "tika-abh06t68:48_0": "1077.",
  "tika-abh06t68:49_0": "Paññapetabbadhammā ca, tesaṁ paññāpitāpi ca;",
  "tika-abh06t68:50_0": "Icchitabbāpi paññattipathā paññattinānatā.",
  "tika-abh06t68:51_0": "1078.",
  "tika-abh06t68:52_0": "Iti vuttānusārena, vuttaṁ aṭṭhakathānaye;",
  "tika-abh06t68:53_0": "Nayaṁ gahetvā etthāpi, paññatti duvidhā katā.",
  "tika-abh06t68:54_0": "1079.",
  "tika-abh06t68:55_0": "Tasmimpi paramatthā ca, saccikaṭṭhasalakkhaṇā;",
  "tika-abh06t68:56_0": "Atthā paññattimattā ca, atthapaññattināmakā.",
  "tika-abh06t68:57_0": "1080.",
  "tika-abh06t68:58_0": "Tesaṁ paññāpikā ceva, nāmapaññattināmikā;",
  "tika-abh06t68:59_0": "Iccevaṁ vaṇṇanāmagge, ñeyyattā tividhā katā.",
  "tika-abh06t68:60_0": "1081.",
  "tika-abh06t68:61_0": "Paramatthasaccaṁ nāma, paramatthāva tattha ca;",
  "tika-abh06t68:62_0": "Saccikaṭṭhasabhāvattā, avisaṁvādakā hi te.",
  "tika-abh06t68:63_0": "1082.",
  "tika-abh06t68:64_0": "Sammutisaccaṁ paññattidvayaṁ vohāravuttiyā;",
  "tika-abh06t68:65_0": "Lokasamaññādhippāyāvisaṁvādakabhāvato.",
  "tika-abh06t68:66_0": "1083.",
  "tika-abh06t68:67_0": "Iti saccadvayampetaṁ, akkhāsi purisuttamo;",
  "tika-abh06t68:68_0": "Tenāpi nāmasaṁviññū, vohareyyubhayampi vā.",
  "tika-abh06t68:69_0": "Iti paññattivibhāge pabhedakathā niṭṭhitā.",
  "tika-abh06t68:70_0": "Sattavīsatimo paricchedo."
}