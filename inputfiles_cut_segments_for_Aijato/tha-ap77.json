{
  "tha-ap77:0.1_0": "Therāpadāna",
  "tha-ap77:0.2_0": "Nāgasamālavagga",
  "tha-ap77:0.3_0": "5. Ekasaññakattheraapadāna",
  "tha-ap77:1.1_0": "“Khaṇḍo nāmāsi nāmena,",
  "tha-ap77:1.2_0": "vipassissaggasāvako;",
  "tha-ap77:1.3_0": "Ekā bhikkhā mayā dinnā,",
  "tha-ap77:1.4_0": "lokāhutipaṭiggahe.",
  "tha-ap77:2.1_0": "Tena cittappasādena,",
  "tha-ap77:2.2_0": "dvipadinda narāsabha;",
  "tha-ap77:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap77:2.4_0": "ekabhikkhāyidaṁ phalaṁ.",
  "tha-ap77:3.1_0": "Cattālīsamhito kappe,",
  "tha-ap77:3.2_0": "varuṇo nāma khattiyo;",
  "tha-ap77:3.3_0": "Sattaratanasampanno,",
  "tha-ap77:3.4_0": "cakkavattī mahabbalo.",
  "tha-ap77:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap77:4.2_0": "…pe…",
  "tha-ap77:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap77:5.1_0": "Itthaṁ sudaṁ āyasmā ekasaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap77:6.1_0": "Ekasaññakattherassāpadānaṁ pañcamaṁ."
}