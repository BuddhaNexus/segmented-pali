{
  "ja465:0.1_0": "Jātaka",
  "ja465:0.2_0": "Dvādasakanipāta",
  "ja465:0.3_0": "Cūḷakuṇālavagga",
  "ja465:0.4_0": "2. Bhaddasālajātaka",
  "ja465:1.1_0": "“Kā tvaṁ suddhehi vatthehi,",
  "ja465:1.2_0": "aghe vehāyasaṁ ṭhitā;",
  "ja465:1.3_0": "Kena tyāssūni vattanti,",
  "ja465:1.4_0": "kuto taṁ bhayamāgataṁ”.",
  "ja465:2.1_0": "“Taveva deva vijite,",
  "ja465:2.2_0": "bhaddasāloti maṁ vidū;",
  "ja465:2.3_0": "Saṭṭhi vassasahassāni,",
  "ja465:2.4_0": "tiṭṭhato pūjitassa me.",
  "ja465:3.1_0": "Kārayantā nagarāni,",
  "ja465:3.2_0": "agāre ca disampati;",
  "ja465:3.3_0": "Vividhe cāpi pāsāde,",
  "ja465:3.4_0": "na maṁ te accamaññisuṁ;",
  "ja465:3.5_0": "Yatheva maṁ te pūjesuṁ,",
  "ja465:3.6_0": "tatheva tvampi pūjaya”.",
  "ja465:4.1_0": "“Taṁ ivāhaṁ na passāmi,",
  "ja465:4.2_0": "thūlaṁ kāyena te dumaṁ;",
  "ja465:4.3_0": "Ārohapariṇāhena,",
  "ja465:4.4_0": "abhirūposi jātiyā.",
  "ja465:5.1_0": "Pāsādaṁ kārayissāmi,",
  "ja465:5.2_0": "ekatthambhaṁ manoramaṁ;",
  "ja465:5.3_0": "Tattha taṁ upanessāmi,",
  "ja465:5.4_0": "ciraṁ te yakkha jīvitaṁ”.",
  "ja465:6.1_0": "“Evaṁ cittaṁ udapādi,",
  "ja465:6.2_0": "sarīrena vinābhāvo;",
  "ja465:6.3_0": "Puthuso maṁ vikantitvā,",
  "ja465:6.4_0": "khaṇḍaso avakantatha.",
  "ja465:7.1_0": "Agge ca chetvā majjhe ca,",
  "ja465:7.2_0": "pacchā mūlamhi chindatha;",
  "ja465:7.3_0": "Evaṁ me chijjamānassa,",
  "ja465:7.4_0": "na dukkhaṁ maraṇaṁ siyā”.",
  "ja465:8.1_0": "“Hatthapādaṁ yathā chinde,",
  "ja465:8.2_0": "Kaṇṇanāsañca jīvato;",
  "ja465:8.3_0": "Tato pacchā siro chinde,",
  "ja465:8.4_0": "Taṁ dukkhaṁ maraṇaṁ siyā.",
  "ja465:9.1_0": "Sukhaṁ nu khaṇḍaso chinnaṁ,",
  "ja465:9.2_0": "bhaddasāla vanappati;",
  "ja465:9.3_0": "Kiṁhetu kiṁ upādāya,",
  "ja465:9.4_0": "khaṇḍaso chinnamicchasi”.",
  "ja465:10.1_0": "“Yañca hetumupādāya,",
  "ja465:10.2_0": "hetuṁ dhammūpasaṁhitaṁ;",
  "ja465:10.3_0": "Khaṇḍaso chinnamicchāmi,",
  "ja465:10.4_0": "mahārāja suṇohi me.",
  "ja465:11.1_0": "Ñātī me sukhasaṁvaddhā,",
  "ja465:11.2_0": "mama passe nivātajā;",
  "ja465:11.3_0": "Tepihaṁ upahiṁseyyaṁ,",
  "ja465:11.4_0": "paresaṁ asukhocitaṁ”.",
  "ja465:12.1_0": "“Ceteyyarūpaṁ cetesi,",
  "ja465:12.2_0": "bhaddasāla vanappati;",
  "ja465:12.3_0": "Hitakāmosi ñātīnaṁ,",
  "ja465:12.4_0": "abhayaṁ samma dammi te”ti.",
  "ja465:13.1_0": "Bhaddasālajātakaṁ dutiyaṁ."
}