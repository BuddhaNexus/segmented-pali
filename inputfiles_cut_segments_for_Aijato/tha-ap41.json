{
  "tha-ap41:0.1_0": "Therāpadāna",
  "tha-ap41:0.2_0": "Kuṇḍadhānavagga",
  "tha-ap41:0.3_0": "9. Dhammacakkikattheraapadāna",
  "tha-ap41:1.1_0": "“Siddhatthassa bhagavato,",
  "tha-ap41:1.2_0": "sīhāsanassa sammukhā;",
  "tha-ap41:1.3_0": "Dhammacakkaṁ me ṭhapitaṁ,",
  "tha-ap41:1.4_0": "sukataṁ viññuvaṇṇitaṁ.",
  "tha-ap41:2.1_0": "Cāruvaṇṇova sobhāmi,",
  "tha-ap41:2.2_0": "sayoggabalavāhano;",
  "tha-ap41:2.3_0": "Parivārenti maṁ niccaṁ,",
  "tha-ap41:2.4_0": "anuyantā bahujjanā.",
  "tha-ap41:3.1_0": "Saṭṭhitūriyasahassehi,",
  "tha-ap41:3.2_0": "paricāremahaṁ sadā;",
  "tha-ap41:3.3_0": "Parivārena sobhāmi,",
  "tha-ap41:3.4_0": "puññakammassidaṁ phalaṁ.",
  "tha-ap41:4.1_0": "Catunnavutito kappe,",
  "tha-ap41:4.2_0": "yaṁ cakkaṁ ṭhapayiṁ ahaṁ;",
  "tha-ap41:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap41:4.4_0": "dhammacakkassidaṁ phalaṁ.",
  "tha-ap41:5.1_0": "Ito ekādase kappe,",
  "tha-ap41:5.2_0": "aṭṭhāsiṁsu janādhipā;",
  "tha-ap41:5.3_0": "Sahassarājanāmena,",
  "tha-ap41:5.4_0": "cakkavattī mahabbalā.",
  "tha-ap41:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap41:6.2_0": "…pe…",
  "tha-ap41:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap41:7.1_0": "Itthaṁ sudaṁ āyasmā dhammacakkiko thero imā gāthāyo abhāsitthāti.",
  "tha-ap41:8.1_0": "Dhammacakkikattherassāpadānaṁ navamaṁ."
}