{
  "tha-ap314:0.1_0": "Therāpadāna",
  "tha-ap314:0.2_0": "Ārakkhadāyakavagga",
  "tha-ap314:0.3_0": "2. Bhojanadāyakattheraapadāna",
  "tha-ap314:1.1_0": "“Sujāto sālalaṭṭhīva,",
  "tha-ap314:1.2_0": "sobhañjanamivuggato;",
  "tha-ap314:1.3_0": "Indalaṭṭhirivākāse,",
  "tha-ap314:1.4_0": "virocati sadā jino.",
  "tha-ap314:2.1_0": "Tassa devātidevassa,",
  "tha-ap314:2.2_0": "vessabhussa mahesino;",
  "tha-ap314:2.3_0": "Adāsi bhojanamahaṁ,",
  "tha-ap314:2.4_0": "vippasannena cetasā.",
  "tha-ap314:3.1_0": "Taṁ me buddho anumodi,",
  "tha-ap314:3.2_0": "sayambhū aparājito;",
  "tha-ap314:3.3_0": "Bhave nibbattamānamhi,",
  "tha-ap314:3.4_0": "phalaṁ nibbattatū tava.",
  "tha-ap314:4.1_0": "Ekattiṁse ito kappe,",
  "tha-ap314:4.2_0": "yaṁ dānamadadiṁ tadā;",
  "tha-ap314:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap314:4.4_0": "bhojanassa idaṁ phalaṁ.",
  "tha-ap314:5.1_0": "Pañcavīse ito kappe,",
  "tha-ap314:5.2_0": "eko āsiṁ amittako;",
  "tha-ap314:5.3_0": "Sattaratanasampanno,",
  "tha-ap314:5.4_0": "cakkavattī mahabbalo.",
  "tha-ap314:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap314:6.2_0": "…pe…",
  "tha-ap314:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap314:7.1_0": "Itthaṁ sudaṁ āyasmā bhojanadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap314:8.1_0": "Bhojanadāyakattherassāpadānaṁ dutiyaṁ."
}