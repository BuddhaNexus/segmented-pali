{
  "kv17.5:0.1_0": "Kathāvatthu",
  "kv17.5:0.2_0": "Mahāpaṇṇāsaka",
  "kv17.5:0.3_0": "Sattarasamavagga",
  "kv17.5:0.4_0": "Ṭhapetvāariyamaggantikathā",
  "kv17.5:1.1_0": "Ṭhapetvā ariyamaggaṁ avasesā saṅkhārā dukkhāti?",
  "kv17.5:1.2_0": "Āmantā.",
  "kv17.5:1.3_0": "Dukkhasamudayopi dukkhoti?",
  "kv17.5:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv17.5:1.5_0": "dukkhasamudayopi dukkhoti?",
  "kv17.5:1.6_0": "Āmantā.",
  "kv17.5:1.7_0": "Tīṇeva ariyasaccānīti?",
  "kv17.5:1.8_0": "Na hevaṁ vattabbe …pe…",
  "kv17.5:1.9_0": "tīṇeva ariyasaccānīti?",
  "kv17.5:1.10_0": "Āmantā.",
  "kv17.5:1.11_0": "Nanu cattāri ariyasaccāni vuttāni bhagavatā—",
  "kv17.5:1.12_0": "dukkhaṁ, dukkhasamudayo, dukkhanirodho, dukkhanirodhagāminī paṭipadāti?",
  "kv17.5:1.13_0": "Āmantā.",
  "kv17.5:1.14_0": "Hañci cattāri ariyasaccāni vuttāni bhagavatā—",
  "kv17.5:1.15_0": "dukkhaṁ, dukkhasamudayo, dukkhanirodho, dukkhanirodhagāminī paṭipadā;",
  "kv17.5:1.16_0": "no ca vata re vattabbe—",
  "kv17.5:1.17_0": "“tīṇeva ariyasaccānī”ti.",
  "kv17.5:2.1_0": "Dukkhasamudayopi dukkhoti?",
  "kv17.5:2.2_0": "Āmantā.",
  "kv17.5:2.3_0": "Kenaṭṭhenāti?",
  "kv17.5:2.4_0": "Aniccaṭṭhena.",
  "kv17.5:2.5_0": "Ariyamaggo aniccoti?",
  "kv17.5:2.6_0": "Āmantā.",
  "kv17.5:2.7_0": "Ariyamaggo dukkhoti?",
  "kv17.5:2.8_0": "Na hevaṁ vattabbe …pe….",
  "kv17.5:3.1_0": "Ariyamaggo anicco, so ca na dukkhoti?",
  "kv17.5:3.2_0": "Āmantā.",
  "kv17.5:3.3_0": "Dukkhasamudayo anicco, so ca na dukkhoti?",
  "kv17.5:3.4_0": "Na hevaṁ vattabbe …pe…",
  "kv17.5:3.5_0": "dukkhasamudayo anicco, so ca dukkhoti?",
  "kv17.5:3.6_0": "Āmantā.",
  "kv17.5:3.7_0": "Ariyamaggo anicco, so ca dukkhoti?",
  "kv17.5:3.8_0": "Na hevaṁ vattabbe …pe….",
  "kv17.5:4.1_0": "Na vattabbaṁ—",
  "kv17.5:4.2_0": "“ṭhapetvā ariyamaggaṁ avasesā saṅkhārā dukkhā”ti?",
  "kv17.5:4.3_0": "Āmantā.",
  "kv17.5:4.4_0": "Nanu sā dukkhanirodhagāminī paṭipadāti?",
  "kv17.5:4.5_0": "Āmantā.",
  "kv17.5:4.6_0": "Hañci sā dukkhanirodhagāminī paṭipadā, tena vata re vattabbe—",
  "kv17.5:4.7_0": "“ṭhapetvā ariyamaggaṁ avasesā saṅkhārā dukkhā”ti.",
  "kv17.5:4.8_0": "Ṭhapetvā ariyamaggantikathā niṭṭhitā."
}