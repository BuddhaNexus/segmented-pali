{
  "ja486:0.1_0": "Jātaka",
  "ja486:0.2_0": "Pakiṇṇakanipāta",
  "ja486:0.3_0": "Sālikedāravagga",
  "ja486:0.4_0": "3. Mahāukkusajātaka",
  "ja486:1.1_0": "“Ukkā cilācā bandhanti dīpe,",
  "ja486:1.2_0": "Pajā mamaṁ khādituṁ patthayanti;",
  "ja486:1.3_0": "Mittaṁ sahāyañca vadehi senaka,",
  "ja486:1.4_0": "Ācikkha ñātibyasanaṁ dijānaṁ”.",
  "ja486:2.1_0": "“Dijo dijānaṁ pavarosi pakkhima,",
  "ja486:2.2_0": "Ukkusarāja saraṇaṁ taṁ upema;",
  "ja486:2.3_0": "Pajā mamaṁ khādituṁ patthayanti,",
  "ja486:2.4_0": "Luddā cilācā bhava me sukhāya”.",
  "ja486:3.1_0": "“Mittaṁ sahāyañca karonti paṇḍitā,",
  "ja486:3.2_0": "Kāle akāle sukhamesamānā;",
  "ja486:3.3_0": "Karomi te senaka etamatthaṁ,",
  "ja486:3.4_0": "Ariyo hi ariyassa karoti kiccaṁ”.",
  "ja486:4.1_0": "“Yaṁ hoti kiccaṁ anukampakena,",
  "ja486:4.2_0": "Ariyassa ariyena kataṁ tayīdaṁ;",
  "ja486:4.3_0": "Attānurakkhī bhava mā adayhi,",
  "ja486:4.4_0": "Lacchāma putte tayi jīvamāne”.",
  "ja486:5.1_0": "“Taveva rakkhāvaraṇaṁ karonto,",
  "ja486:5.2_0": "Sarīrabhedāpi na santasāmi;",
  "ja486:5.3_0": "Karonti heke sakhīnaṁ sakhāro,",
  "ja486:5.4_0": "Pāṇaṁ cajantā satamesa dhammo”.",
  "ja486:6.1_0": "“Sudukkaraṁ kammamakāsi,",
  "ja486:6.2_0": "aṇḍajāyaṁ vihaṅgamo;",
  "ja486:6.3_0": "Atthāya kuraro putte,",
  "ja486:6.4_0": "aḍḍharatte anāgate”.",
  "ja486:7.1_0": "“Cutāpi heke khalitā sakammunā,",
  "ja486:7.2_0": "Mittānukampāya patiṭṭhahanti;",
  "ja486:7.3_0": "Puttā mamaṭṭā gatimāgatosmi,",
  "ja486:7.4_0": "Atthaṁ caretho mama vāricara”.",
  "ja486:8.1_0": "“Dhanena dhaññena ca attanā ca,",
  "ja486:8.2_0": "Mittaṁ sahāyañca karonti paṇḍitā;",
  "ja486:8.3_0": "Karomi te senaka etamatthaṁ,",
  "ja486:8.4_0": "Ariyo hi ariyassa karoti kiccaṁ”.",
  "ja486:9.1_0": "“Appossukko tāta tuvaṁ nisīda,",
  "ja486:9.2_0": "Putto pitu carati atthacariyaṁ;",
  "ja486:9.3_0": "Ahaṁ carissāmi tavetamatthaṁ,",
  "ja486:9.4_0": "Senassa putte paritāyamāno”.",
  "ja486:10.1_0": "“Addhā hi tāta satamesa dhammo,",
  "ja486:10.2_0": "Putto pitu yaṁ care atthacariyaṁ;",
  "ja486:10.3_0": "Appeva maṁ disvāna pavaḍḍhakāyaṁ,",
  "ja486:10.4_0": "Senassa puttā na viheṭhayeyyuṁ”.",
  "ja486:11.1_0": "“Pasū manussā migavīraseṭṭha,",
  "ja486:11.2_0": "Bhayaṭṭitā seṭṭhamupabbajanti;",
  "ja486:11.3_0": "Puttā mamaṭṭā gatimāgatosmi,",
  "ja486:11.4_0": "Tvaṁ nosi rājā bhava me sukhāya”.",
  "ja486:12.1_0": "“Karomi te senaka etamatthaṁ,",
  "ja486:12.2_0": "Āyāmi te taṁ disataṁ vadhāya;",
  "ja486:12.3_0": "Kathañhi viññū pahu sampajāno,",
  "ja486:12.4_0": "Na vāyame attajanassa guttiyā”.",
  "ja486:13.1_0": "“Mittañca kayirātha suhadayañca,",
  "ja486:13.2_0": "Ayirañca kayirātha sukhāgamāya;",
  "ja486:13.3_0": "Nivatthakocova sarebhihantvā,",
  "ja486:13.4_0": "Modāma puttehi samaṅgibhūtā.",
  "ja486:14.1_0": "Sakamittassa kammena,",
  "ja486:14.2_0": "sahāyassāpalāyino;",
  "ja486:14.3_0": "Kūjantamupakūjanti,",
  "ja486:14.4_0": "lomasā hadayaṅgamaṁ.",
  "ja486:15.1_0": "Mittaṁ sahāyaṁ adhigamma paṇḍito,",
  "ja486:15.2_0": "So bhuñjatī putta pasuṁ dhanaṁ vā;",
  "ja486:15.3_0": "Ahañca puttā ca patī ca mayhaṁ,",
  "ja486:15.4_0": "Mittānukampāya samaṅgibhūtā.",
  "ja486:16.1_0": "Rājavatā sūravatā ca attho,",
  "ja486:16.2_0": "Sampannasakhissa bhavanti hete;",
  "ja486:16.3_0": "So mittavā yasavā uggatatto,",
  "ja486:16.4_0": "Asmiṁdhaloke modati kāmakāmī.",
  "ja486:17.1_0": "Karaṇīyāni mittāni,",
  "ja486:17.2_0": "daliddenāpi senaka;",
  "ja486:17.3_0": "Passa mittānukampāya,",
  "ja486:17.4_0": "samaggamhā sañātake.",
  "ja486:18.1_0": "Sūrena balavantena,",
  "ja486:18.2_0": "yo mitte kurute dijo;",
  "ja486:18.3_0": "Evaṁ so sukhito hoti,",
  "ja486:18.4_0": "yathāhaṁ tvañca senakāti”.",
  "ja486:19.1_0": "Mahāukkusajātakaṁ tatiyaṁ."
}