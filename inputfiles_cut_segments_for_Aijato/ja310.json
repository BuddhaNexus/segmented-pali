{
  "ja310:0.1_0": "Jātaka",
  "ja310:0.2_0": "Catukkanipāta",
  "ja310:0.3_0": "Kāliṅgavagga",
  "ja310:0.4_0": "10. Seyyajātaka",
  "ja310:1.1_0": "“Sasamuddapariyāyaṁ,",
  "ja310:1.2_0": "mahiṁ sāgarakuṇḍalaṁ;",
  "ja310:1.3_0": "Na icche saha nindāya,",
  "ja310:1.4_0": "evaṁ seyya vijānahi.",
  "ja310:2.1_0": "Dhiratthu taṁ yasalābhaṁ,",
  "ja310:2.2_0": "dhanalābhañca brāhmaṇa;",
  "ja310:2.3_0": "Yā vutti vinipātena,",
  "ja310:2.4_0": "adhammacaraṇena vā.",
  "ja310:3.1_0": "Api ce pattamādāya,",
  "ja310:3.2_0": "anagāro paribbaje;",
  "ja310:3.3_0": "Sāyeva jīvikā seyyo,",
  "ja310:3.4_0": "yā cādhammena esanā.",
  "ja310:4.1_0": "Api ce pattamādāya,",
  "ja310:4.2_0": "anagāro paribbaje;",
  "ja310:4.3_0": "Aññaṁ ahiṁsayaṁ loke,",
  "ja310:4.4_0": "api rajjena taṁ varan”ti.",
  "ja310:5.1_0": "Seyyajātakaṁ dasamaṁ.",
  "ja310:5.2_0": "Kāliṅgavaggo paṭhamo.",
  "ja310:6.1_0": "Tassuddānaṁ",
  "ja310:7.1_0": "Vivarañca adeyya samiddhavaraṁ,",
  "ja310:7.2_0": "Atha daddara pāpamahātiraho;",
  "ja310:7.3_0": "Atha koli palāsavarañca kara,",
  "ja310:7.4_0": "Carimaṁ sasamuddavarena dasāti."
}