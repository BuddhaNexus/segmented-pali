{
  "tha-ap517:0.1_0": "Therāpadāna",
  "tha-ap517:0.2_0": "Phaladāyakavagga",
  "tha-ap517:0.3_0": "5. Nāgapupphiyattheraapadāna",
  "tha-ap517:1.1_0": "“Suvaṇṇavaṇṇaṁ sambuddhaṁ,",
  "tha-ap517:1.2_0": "Āhutīnaṁ paṭiggahaṁ;",
  "tha-ap517:1.3_0": "Rathiyaṁ paṭipajjantaṁ,",
  "tha-ap517:1.4_0": "Nāgapupphaṁ apūjayiṁ.",
  "tha-ap517:2.1_0": "Ekanavutito kappe,",
  "tha-ap517:2.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap517:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap517:2.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap517:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap517:3.2_0": "…pe…",
  "tha-ap517:3.3_0": "viharāmi anāsavo.",
  "tha-ap517:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap517:4.2_0": "…pe…",
  "tha-ap517:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap517:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap517:5.2_0": "…pe…",
  "tha-ap517:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap517:6.1_0": "Itthaṁ sudaṁ āyasmā nāgapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap517:7.1_0": "Nāgapupphiyattherassāpadānaṁ pañcamaṁ."
}