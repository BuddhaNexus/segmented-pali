{
  "ja141:0.1_0": "Jātaka",
  "ja141:0.2_0": "Ekakanipāta",
  "ja141:0.3_0": "Kakaṇṭakavagga",
  "ja141:0.4_0": "1. Godhajātaka",
  "ja141:1.1_0": "“Na pāpajanasaṁsevī,",
  "ja141:1.2_0": "accantasukhamedhati;",
  "ja141:1.3_0": "Godhākulaṁ kakaṇṭāva,",
  "ja141:1.4_0": "kaliṁ pāpeti attanan”ti.",
  "ja141:2.1_0": "Godhajātakaṁ paṭhamaṁ."
}