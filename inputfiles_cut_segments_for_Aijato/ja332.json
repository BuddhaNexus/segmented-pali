{
  "ja332:0.1_0": "Jātaka",
  "ja332:0.2_0": "Catukkanipāta",
  "ja332:0.3_0": "Kokilavagga",
  "ja332:0.4_0": "2. Rathalaṭṭhijātaka",
  "ja332:1.1_0": "“Api hantvā hato brūti,",
  "ja332:1.2_0": "jetvā jitoti bhāsati;",
  "ja332:1.3_0": "Pubbamakkhāyino rāja,",
  "ja332:1.4_0": "aññadatthu na saddahe.",
  "ja332:2.1_0": "Tasmā paṇḍitajātiyo,",
  "ja332:2.2_0": "suṇeyya itarassapi;",
  "ja332:2.3_0": "Ubhinnaṁ vacanaṁ sutvā,",
  "ja332:2.4_0": "yathā dhammo tathā kare.",
  "ja332:3.1_0": "Alaso gihī kāmabhogī na sādhu,",
  "ja332:3.2_0": "Asaññato pabbajito na sādhu;",
  "ja332:3.3_0": "Rājā na sādhu anisammakārī,",
  "ja332:3.4_0": "Yo paṇḍito kodhano taṁ na sādhu.",
  "ja332:4.1_0": "Nisamma khattiyo kayirā,",
  "ja332:4.2_0": "nānisamma disampati;",
  "ja332:4.3_0": "Nisammakārino rāja,",
  "ja332:4.4_0": "yaso kitti ca vaḍḍhatī”ti.",
  "ja332:5.1_0": "Rathalaṭṭhijātakaṁ dutiyaṁ."
}