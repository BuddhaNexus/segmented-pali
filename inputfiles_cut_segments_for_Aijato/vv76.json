{
  "vv76:0.1_0": "Vimānavatthu",
  "vv76:0.2_0": "Purisavimāna",
  "vv76:0.3_0": "Sunikkhittavagga",
  "vv76:0.4_0": "2. Nandanavimānavatthu",
  "vv76:1.1_0": "“Yathā vanaṁ nandanaṁ pabhāsati,",
  "vv76:1.2_0": "Uyyānaseṭṭhaṁ tidasānamuttamaṁ;",
  "vv76:1.3_0": "Tathūpamaṁ tuyhamidaṁ vimānaṁ,",
  "vv76:1.4_0": "Obhāsayaṁ tiṭṭhati antalikkhe.",
  "vv76:2.1_0": "Deviddhipattosi mahānubhāvo,",
  "vv76:2.2_0": "Manussabhūto kimakāsi puññaṁ;",
  "vv76:2.3_0": "Kenāsi evaṁ jalitānubhāvo,",
  "vv76:2.4_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv76:3.1_0": "So devaputto attamano,",
  "vv76:3.2_0": "…pe…",
  "vv76:3.3_0": "yassa kammassidaṁ phalaṁ.",
  "vv76:4.1_0": "“Ahaṁ manussesu manussabhūto,",
  "vv76:4.2_0": "Daliddo atāṇo kapaṇo kammakaro ahosiṁ;",
  "vv76:4.3_0": "Jiṇṇe ca mātāpitaro abhāriṁ,",
  "vv76:4.4_0": "Piyā ca me sīlavanto ahesuṁ;",
  "vv76:4.5_0": "Annañca pānañca pasannacitto,",
  "vv76:4.6_0": "Sakkacca dānaṁ vipulaṁ adāsiṁ.",
  "vv76:5.1_0": "Tena metādiso vaṇṇo,",
  "vv76:5.2_0": "…pe…",
  "vv76:5.3_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv76:6.1_0": "Nandanavimānaṁ dutiyaṁ."
}