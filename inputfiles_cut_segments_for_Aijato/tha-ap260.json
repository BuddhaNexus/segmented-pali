{
  "tha-ap260:0.1_0": "Therāpadāna",
  "tha-ap260:0.2_0": "Thomakavagga",
  "tha-ap260:0.3_0": "8. Paṇṇadāyakattheraapadāna",
  "tha-ap260:1.1_0": "“Pabbate himavantamhi,",
  "tha-ap260:1.2_0": "vākacīradharo ahaṁ;",
  "tha-ap260:1.3_0": "Aloṇapaṇṇabhakkhomhi,",
  "tha-ap260:1.4_0": "niyamesu ca saṁvuto.",
  "tha-ap260:2.1_0": "Pātarāse anuppatte,",
  "tha-ap260:2.2_0": "siddhattho upagacchi maṁ;",
  "tha-ap260:2.3_0": "Tāhaṁ buddhassa pādāsiṁ,",
  "tha-ap260:2.4_0": "pasanno sehi pāṇibhi.",
  "tha-ap260:3.1_0": "Catunnavutito kappe,",
  "tha-ap260:3.2_0": "yaṁ paṇṇamadadiṁ tadā;",
  "tha-ap260:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap260:3.4_0": "paṇṇadānassidaṁ phalaṁ.",
  "tha-ap260:4.1_0": "Sattavīsatikappamhi,",
  "tha-ap260:4.2_0": "rājā āsiṁ sadatthiyo;",
  "tha-ap260:4.3_0": "Sattaratanasampanno,",
  "tha-ap260:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap260:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap260:5.2_0": "…pe…",
  "tha-ap260:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap260:6.1_0": "Itthaṁ sudaṁ āyasmā paṇṇadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap260:7.1_0": "Paṇṇadāyakattherassāpadānaṁ aṭṭhamaṁ."
}