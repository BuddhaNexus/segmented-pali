{
  "tha-ap234:0.1_0": "Therāpadāna",
  "tha-ap234:0.2_0": "Udakāsanavagga",
  "tha-ap234:0.3_0": "2 Bhājanapālakattheraapadāna",
  "tha-ap234:1.1_0": "“Nagare bandhumatiyā,",
  "tha-ap234:1.2_0": "kumbhakāro ahaṁ tadā;",
  "tha-ap234:1.3_0": "Bhājanaṁ anupālesiṁ,",
  "tha-ap234:1.4_0": "bhikkhusaṅghassa tāvade.",
  "tha-ap234:2.1_0": "Ekanavutito kappe,",
  "tha-ap234:2.2_0": "bhājanaṁ anupālayiṁ;",
  "tha-ap234:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap234:2.4_0": "bhājanassa idaṁ phalaṁ.",
  "tha-ap234:3.1_0": "Tepaññāse ito kappe,",
  "tha-ap234:3.2_0": "anantajālināmako;",
  "tha-ap234:3.3_0": "Sattaratanasampanno,",
  "tha-ap234:3.4_0": "cakkavattī mahabbalo.",
  "tha-ap234:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap234:4.2_0": "…pe…",
  "tha-ap234:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap234:5.1_0": "Itthaṁ sudaṁ āyasmā bhājanapālako thero imā gāthāyo abhāsitthāti.",
  "tha-ap234:6.1_0": "Bhājanapālakattherassāpadānaṁ dutiyaṁ."
}