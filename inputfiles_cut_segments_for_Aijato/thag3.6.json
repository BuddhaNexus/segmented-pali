{
  "thag3.6:0.1_0": "Theragāthā",
  "thag3.6:0.2_0": "Tikanipāta",
  "thag3.6:0.3_0": "Paṭhamavagga",
  "thag3.6:0.4_0": "3.6. Khujjasobhitattheragāthā",
  "thag3.6:1.1_0": "“Ye cittakathī bahussutā,",
  "thag3.6:1.2_0": "Samaṇā pāṭaliputtavāsino;",
  "thag3.6:1.3_0": "Tesaññataroyamāyuvā,",
  "thag3.6:1.4_0": "Dvāre tiṭṭhati khujjasobhito”.",
  "thag3.6:2.1_0": "“Ye cittakathī bahussutā,",
  "thag3.6:2.2_0": "Samaṇā pāṭaliputtavāsino;",
  "thag3.6:2.3_0": "Tesaññataroyamāyuvā,",
  "thag3.6:2.4_0": "Dvāre tiṭṭhati māluterito”.",
  "thag3.6:3.1_0": "“Suyuddhena suyiṭṭhena,",
  "thag3.6:3.2_0": "saṅgāmavijayena ca;",
  "thag3.6:3.3_0": "Brahmacariyānuciṇṇena,",
  "thag3.6:3.4_0": "evāyaṁ sukhamedhatī”ti.",
  "thag3.6:3.5_0": "… Khujjasobhito thero …."
}