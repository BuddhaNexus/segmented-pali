{
  "vv27:0.1_0": "Vimānavatthu",
  "vv27:0.2_0": "Itthivimāna",
  "vv27:0.3_0": "Cittalatāvagga",
  "vv27:0.4_0": "10. Paṭhamabhikkhādāyikāvimānavatthu",
  "vv27:1.1_0": "“Abhikkantena vaṇṇena,",
  "vv27:1.2_0": "yā tvaṁ tiṭṭhasi devate;",
  "vv27:1.3_0": "Obhāsentī disā sabbā,",
  "vv27:1.4_0": "osadhī viya tārakā.",
  "vv27:2.1_0": "Kena tetādiso vaṇṇo,",
  "vv27:2.2_0": "…pe…",
  "vv27:2.3_0": "vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv27:3.1_0": "Sā devatā attamanā,",
  "vv27:3.2_0": "…pe…",
  "vv27:3.3_0": "yassa kammassidaṁ phalaṁ.",
  "vv27:4.1_0": "“Ahaṁ manussesu manussabhūtā,",
  "vv27:4.2_0": "Purimāya jātiyā manussaloke.",
  "vv27:5.1_0": "Addasaṁ virajaṁ buddhaṁ,",
  "vv27:5.2_0": "vippasannamanāvilaṁ;",
  "vv27:5.3_0": "Tassa adāsahaṁ bhikkhaṁ,",
  "vv27:5.4_0": "pasannā sehi pāṇibhi.",
  "vv27:6.1_0": "Tena metādiso vaṇṇo,",
  "vv27:6.2_0": "…pe…",
  "vv27:6.3_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv27:7.1_0": "Paṭhamabhikkhādāyikāvimānaṁ dasamaṁ."
}