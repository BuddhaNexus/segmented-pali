{
  "tha-ap512:0.1_0": "Therāpadāna",
  "tha-ap512:0.2_0": "Kaṇikāravagga",
  "tha-ap512:0.3_0": "10 Nāḷikeraphaladāyakattheraapadāna",
  "tha-ap512:1.1_0": "“Nagare bandhumatiyā,",
  "tha-ap512:1.2_0": "ārāmiko ahaṁ tadā;",
  "tha-ap512:1.3_0": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap512:1.4_0": "gacchantaṁ anilañjase.",
  "tha-ap512:2.1_0": "Nāḷikeraphalaṁ gayha,",
  "tha-ap512:2.2_0": "buddhaseṭṭhassadāsahaṁ;",
  "tha-ap512:2.3_0": "Ākāse ṭhitako santo,",
  "tha-ap512:2.4_0": "paṭiggaṇhi mahāyaso.",
  "tha-ap512:3.1_0": "Vittisañjanano mayhaṁ,",
  "tha-ap512:3.2_0": "diṭṭhadhammasukhāvaho;",
  "tha-ap512:3.3_0": "Phalaṁ buddhassa datvāna,",
  "tha-ap512:3.4_0": "vippasannena cetasā.",
  "tha-ap512:4.1_0": "Adhigacchiṁ tadā pītiṁ,",
  "tha-ap512:4.2_0": "vipulañca sukhuttamaṁ;",
  "tha-ap512:4.3_0": "Uppajjateva ratanaṁ,",
  "tha-ap512:4.4_0": "nibbattassa tahiṁ tahiṁ.",
  "tha-ap512:5.1_0": "Ekanavutito kappe,",
  "tha-ap512:5.2_0": "yaṁ phalaṁ adadiṁ tadā;",
  "tha-ap512:5.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap512:5.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap512:6.1_0": "Dibbacakkhu visuddhaṁ me,",
  "tha-ap512:6.2_0": "samādhikusalo ahaṁ;",
  "tha-ap512:6.3_0": "Abhiññāpāramippatto,",
  "tha-ap512:6.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap512:7.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap512:7.2_0": "…pe…",
  "tha-ap512:7.3_0": "viharāmi anāsavo.",
  "tha-ap512:8.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap512:8.2_0": "…pe…",
  "tha-ap512:8.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap512:9.1_0": "Paṭisambhidā catasso,",
  "tha-ap512:9.2_0": "…pe…",
  "tha-ap512:9.3_0": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap512:10.1_0": "Itthaṁ sudaṁ āyasmā nāḷikeraphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap512:11.1_0": "Nāḷikeraphaladāyakattherassāpadānaṁ dasamaṁ.",
  "tha-ap512:12.1_0": "Kaṇikāravaggo ekapaññāsamo.",
  "tha-ap512:13.1_0": "Tassuddānaṁ",
  "tha-ap512:14.1_0": "Kaṇikārekapattā ca,",
  "tha-ap512:14.2_0": "kāsumārī tathāvaṭā;",
  "tha-ap512:14.3_0": "Pādañca mātuluṅgañca,",
  "tha-ap512:14.4_0": "ajelīmodameva ca.",
  "tha-ap512:15.1_0": "Tālaṁ tathā nāḷikeraṁ,",
  "tha-ap512:15.2_0": "gāthāyo gaṇitā viha;",
  "tha-ap512:15.3_0": "Ekaṁ gāthāsataṁ hoti,",
  "tha-ap512:15.4_0": "ūnādhikavivajjitaṁ."
}