{
  "bv25:0.1_0": "Buddhavaṁsa",
  "bv25:0.2_0": "25. Koṇāgamanabuddhavaṁsa",
  "bv25:1.1_0": "“Kakusandhassa aparena,",
  "bv25:1.2_0": "sambuddho dvipaduttamo;",
  "bv25:1.3_0": "Koṇāgamano nāma jino,",
  "bv25:1.4_0": "lokajeṭṭho narāsabho.",
  "bv25:2.1_0": "Dasa dhamme pūrayitvāna,",
  "bv25:2.2_0": "kantāraṁ samatikkami;",
  "bv25:2.3_0": "Pavāhiya malaṁ sabbaṁ,",
  "bv25:2.4_0": "patto sambodhimuttamaṁ.",
  "bv25:3.1_0": "Dhammacakkaṁ pavattente,",
  "bv25:3.2_0": "koṇāgamananāyake;",
  "bv25:3.3_0": "Tiṁsakoṭisahassānaṁ,",
  "bv25:3.4_0": "paṭhamābhisamayo ahu.",
  "bv25:4.1_0": "Pāṭihīraṁ karonte ca,",
  "bv25:4.2_0": "paravādappamaddane;",
  "bv25:4.3_0": "Vīsatikoṭisahassānaṁ,",
  "bv25:4.4_0": "dutiyābhisamayo ahu.",
  "bv25:5.1_0": "Tato vikubbanaṁ katvā,",
  "bv25:5.2_0": "jino devapuraṁ gato;",
  "bv25:5.3_0": "Vasate tattha sambuddho,",
  "bv25:5.4_0": "silāya paṇḍukambale.",
  "bv25:6.1_0": "Pakaraṇe satta desento,",
  "bv25:6.2_0": "vassaṁ vasati so muni;",
  "bv25:6.3_0": "Dasakoṭisahassānaṁ,",
  "bv25:6.4_0": "tatiyābhisamayo ahu.",
  "bv25:7.1_0": "Tassāpi devadevassa,",
  "bv25:7.2_0": "eko āsi samāgamo;",
  "bv25:7.3_0": "Khīṇāsavānaṁ vimalānaṁ,",
  "bv25:7.4_0": "santacittāna tādinaṁ.",
  "bv25:8.1_0": "Tiṁsabhikkhusahassānaṁ,",
  "bv25:8.2_0": "tadā āsi samāgamo;",
  "bv25:8.3_0": "Oghānamatikkantānaṁ,",
  "bv25:8.4_0": "bhijjitānañca maccuyā.",
  "bv25:9.1_0": "Ahaṁ tena samayena,",
  "bv25:9.2_0": "pabbato nāma khattiyo;",
  "bv25:9.3_0": "Mittāmaccehi sampanno,",
  "bv25:9.4_0": "anantabalavāhano.",
  "bv25:10.1_0": "Sambuddhadassanaṁ gantvā,",
  "bv25:10.2_0": "sutvā dhammamanuttaraṁ;",
  "bv25:10.3_0": "Nimantetvā sajinasaṅghaṁ,",
  "bv25:10.4_0": "dānaṁ datvā yadicchakaṁ.",
  "bv25:11.1_0": "Pattuṇṇaṁ cīnapaṭṭañca,",
  "bv25:11.2_0": "koseyyaṁ kambalampi ca;",
  "bv25:11.3_0": "Sovaṇṇapādukañceva,",
  "bv25:11.4_0": "adāsiṁ satthusāvake.",
  "bv25:12.1_0": "Sopi maṁ buddho byākāsi,",
  "bv25:12.2_0": "saṅghamajjhe nisīdiya;",
  "bv25:12.3_0": "‘Imamhi bhaddake kappe,",
  "bv25:12.4_0": "ayaṁ buddho bhavissati.",
  "bv25:13.1_0": "Ahu kapilavhayā rammā,",
  "bv25:13.2_0": "…pe…",
  "bv25:13.3_0": "hessāma sammukhā imaṁ’.",
  "bv25:14.1_0": "Tassāpi vacanaṁ sutvā,",
  "bv25:14.2_0": "bhiyyo cittaṁ pasādayiṁ;",
  "bv25:14.3_0": "Uttariṁ vatamadhiṭṭhāsiṁ,",
  "bv25:14.4_0": "dasapāramipūriyā.",
  "bv25:15.1_0": "Sabbaññutaṁ gavesanto,",
  "bv25:15.2_0": "dānaṁ datvā naruttame;",
  "bv25:15.3_0": "Ohāyāhaṁ mahārajjaṁ,",
  "bv25:15.4_0": "pabbajiṁ jinasantike.",
  "bv25:16.1_0": "Nagaraṁ sobhavatī nāma,",
  "bv25:16.2_0": "sobho nāmāsi khattiyo;",
  "bv25:16.3_0": "Vasate tattha nagare,",
  "bv25:16.4_0": "sambuddhassa mahākulaṁ.",
  "bv25:17.1_0": "Brāhmaṇo yaññadatto ca,",
  "bv25:17.2_0": "āsi buddhassa so pitā;",
  "bv25:17.3_0": "Uttarā nāma janikā,",
  "bv25:17.4_0": "koṇāgamanassa satthuno.",
  "bv25:18.1_0": "Tīṇi vassasahassāni,",
  "bv25:18.2_0": "Agāraṁ ajjha so vasi;",
  "bv25:18.3_0": "Tusitasantusitasantuṭṭhā,",
  "bv25:18.4_0": "Tayo pāsādamuttamā.",
  "bv25:19.1_0": "Anūnasoḷasasahassāni,",
  "bv25:19.2_0": "Nāriyo samalaṅkatā;",
  "bv25:19.3_0": "Rucigattā nāma nārī,",
  "bv25:19.4_0": "Satthavāho nāma atrajo.",
  "bv25:20.1_0": "Nimitte caturo disvā,",
  "bv25:20.2_0": "hatthiyānena nikkhami;",
  "bv25:20.3_0": "Chamāsaṁ padhānacāraṁ,",
  "bv25:20.4_0": "acarī purisuttamo.",
  "bv25:21.1_0": "Brahmunā yācito santo,",
  "bv25:21.2_0": "koṇāgamananāyako;",
  "bv25:21.3_0": "Vatti cakkaṁ mahāvīro,",
  "bv25:21.4_0": "migadāye naruttamo.",
  "bv25:22.1_0": "Bhiyyaso uttaro nāma,",
  "bv25:22.2_0": "ahesuṁ aggasāvakā;",
  "bv25:22.3_0": "Sotthijo nāmupaṭṭhāko,",
  "bv25:22.4_0": "koṇāgamanassa satthuno.",
  "bv25:23.1_0": "Samuddā uttarā ceva,",
  "bv25:23.2_0": "ahesuṁ aggasāvikā;",
  "bv25:23.3_0": "Bodhi tassa bhagavato,",
  "bv25:23.4_0": "udumbaroti pavuccati.",
  "bv25:24.1_0": "Uggo ca somadevo ca,",
  "bv25:24.2_0": "ahesuṁ aggupaṭṭhakā;",
  "bv25:24.3_0": "Sīvalā ceva sāmā ca,",
  "bv25:24.4_0": "ahesuṁ aggupaṭṭhikā.",
  "bv25:25.1_0": "Uccattanena so buddho,",
  "bv25:25.2_0": "tiṁsahatthasamuggato;",
  "bv25:25.3_0": "Ukkāmukhe yathā kambu,",
  "bv25:25.4_0": "evaṁ raṁsīhi maṇḍito.",
  "bv25:26.1_0": "Tiṁsavassasahassāni,",
  "bv25:26.2_0": "āyu buddhassa tāvade;",
  "bv25:26.3_0": "Tāvatā tiṭṭhamāno so,",
  "bv25:26.4_0": "tāresi janataṁ bahuṁ.",
  "bv25:27.1_0": "Dhammacetiṁ samussetvā,",
  "bv25:27.2_0": "dhammadussavibhūsitaṁ;",
  "bv25:27.3_0": "Dhammapupphaguḷaṁ katvā,",
  "bv25:27.4_0": "nibbuto so sasāvako.",
  "bv25:28.1_0": "Mahāvilāso tassa jano,",
  "bv25:28.2_0": "siridhammappakāsano;",
  "bv25:28.3_0": "Sabbaṁ tamantarahitaṁ,",
  "bv25:28.4_0": "nanu rittā sabbasaṅkhārā.",
  "bv25:29.1_0": "Koṇāgamano sambuddho,",
  "bv25:29.2_0": "Pabbatārāmamhi nibbuto;",
  "bv25:29.3_0": "Dhātuvitthārikaṁ āsi,",
  "bv25:29.4_0": "Tesu tesu padesato”ti. ",
  "bv25:30.1_0": "Koṇāgamanassa bhagavato vaṁso tevīsatimo."
}