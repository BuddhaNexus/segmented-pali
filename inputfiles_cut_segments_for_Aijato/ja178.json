{
  "ja178:0.1_0": "Jātaka",
  "ja178:0.2_0": "Dukanipāta",
  "ja178:0.3_0": "Kalyāṇavagga",
  "ja178:0.4_0": "8. Kacchapajātaka",
  "ja178:1.1_0": "“Janittaṁ me bhavittaṁ me,",
  "ja178:1.2_0": "iti paṅke avassayiṁ;",
  "ja178:1.3_0": "Taṁ maṁ paṅko ajjhabhavi,",
  "ja178:1.4_0": "yathā dubbalakaṁ tathā;",
  "ja178:1.5_0": "Taṁ taṁ vadāmi bhaggava,",
  "ja178:1.6_0": "suṇohi vacanaṁ mama”.",
  "ja178:2.1_0": "“Gāme vā yadi vā raññe,",
  "ja178:2.2_0": "sukhaṁ yatrādhigacchati;",
  "ja178:2.3_0": "Taṁ janittaṁ bhavittañca,",
  "ja178:2.4_0": "purisassa pajānato;",
  "ja178:2.5_0": "Yamhi jīve tamhi gacche,",
  "ja178:2.6_0": "na niketahato siyā”ti.",
  "ja178:3.1_0": "Kacchapajātakaṁ aṭṭhamaṁ."
}