{
  "ja450:0.1_0": "Jātaka",
  "ja450:0.2_0": "Dasakanipāta",
  "ja450:0.3_0": "Catudvāravagga",
  "ja450:0.4_0": "12. Bilārakosiyajātaka",
  "ja450:1.1_0": "“Apacantāpi dicchanti,",
  "ja450:1.2_0": "santo laddhāna bhojanaṁ;",
  "ja450:1.3_0": "Kimeva tvaṁ pacamāno,",
  "ja450:1.4_0": "yaṁ na dajjā na taṁ samaṁ.",
  "ja450:2.1_0": "Maccherā ca pamādā ca,",
  "ja450:2.2_0": "evaṁ dānaṁ na diyyati;",
  "ja450:2.3_0": "Puññaṁ ākaṅkhamānena,",
  "ja450:2.4_0": "deyyaṁ hoti vijānatā.",
  "ja450:3.1_0": "Yasseva bhīto na dadāti maccharī,",
  "ja450:3.2_0": "Tadevādadato bhayaṁ;",
  "ja450:3.3_0": "Jighacchā ca pipāsā ca,",
  "ja450:3.4_0": "Yassa bhāyati maccharī;",
  "ja450:3.5_0": "Tameva bālaṁ phusati,",
  "ja450:3.6_0": "Asmiṁ loke paramhi ca.",
  "ja450:4.1_0": "Tasmā vineyya maccheraṁ,",
  "ja450:4.2_0": "dajjā dānaṁ malābhibhū;",
  "ja450:4.3_0": "Puññāni paralokasmiṁ,",
  "ja450:4.4_0": "patiṭṭhā honti pāṇinaṁ”.",
  "ja450:5.1_0": "“Duddadaṁ dadamānānaṁ,",
  "ja450:5.2_0": "dukkaraṁ kamma kubbataṁ;",
  "ja450:5.3_0": "Asanto nānukubbanti,",
  "ja450:5.4_0": "sataṁ dhammo durannayo.",
  "ja450:6.1_0": "Tasmā satañca asataṁ,",
  "ja450:6.2_0": "nānā hoti ito gati;",
  "ja450:6.3_0": "Asanto nirayaṁ yanti,",
  "ja450:6.4_0": "santo saggaparāyaṇā”.",
  "ja450:7.1_0": "“Appasmeke pavecchanti,",
  "ja450:7.2_0": "bahuneke na dicchare;",
  "ja450:7.3_0": "Appasmā dakkhiṇā dinnā,",
  "ja450:7.4_0": "sahassena samaṁ mitā”.",
  "ja450:8.1_0": "“Dhammaṁ care yopi samuñchakaṁ care,",
  "ja450:8.2_0": "Dārañca posaṁ dadamappakasmiṁ;",
  "ja450:8.3_0": "Sataṁ sahassānaṁ sahassayāginaṁ,",
  "ja450:8.4_0": "Kalampi nāgghanti tathāvidhassa te”.",
  "ja450:9.1_0": "“Kenesa yañño vipulo mahagghato,",
  "ja450:9.2_0": "Samena dinnassa na agghameti;",
  "ja450:9.3_0": "Kathaṁ sataṁ sahassānaṁ sahassayāginaṁ,",
  "ja450:9.4_0": "Kalampi nāgghanti tathāvidhassa te”.",
  "ja450:10.1_0": "“Dadanti heke visame niviṭṭhā,",
  "ja450:10.2_0": "Chetvā vadhitvā atha socayitvā;",
  "ja450:10.3_0": "Sā dakkhiṇā assumukhā sadaṇḍā,",
  "ja450:10.4_0": "Samena dinnassa na agghameti;",
  "ja450:10.5_0": "Evaṁ sataṁ sahassānaṁ sahassayāginaṁ,",
  "ja450:10.6_0": "Kalampi nāgghanti tathāvidhassa te”ti.",
  "ja450:11.1_0": "Bilārakosiyajātakaṁ dvādasamaṁ."
}