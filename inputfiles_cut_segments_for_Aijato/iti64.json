{
  "iti64:0.1_0": "Itivuttaka 64",
  "iti64:0.2_0": "Tikanipāta",
  "iti64:0.3_0": "Dutiyavagga",
  "iti64:0.4_0": "Duccaritasutta",
  "iti64:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti64:2.1_0": "“Tīṇimāni, bhikkhave, duccaritāni.",
  "iti64:2.2_0": "Katamāni tīṇi?",
  "iti64:2.3_0": "Kāyaduccaritaṁ, vacīduccaritaṁ, manoduccaritaṁ—",
  "iti64:2.4_0": "imāni kho, bhikkhave, tīṇi duccaritānī”ti.",
  "iti64:2.5_0": "Etamatthaṁ bhagavā avoca.",
  "iti64:2.6_0": "Tatthetaṁ iti vuccati:",
  "iti64:3.1_0": "“Kāyaduccaritaṁ katvā,",
  "iti64:3.2_0": "vacīduccaritāni ca;",
  "iti64:3.3_0": "Manoduccaritaṁ katvā,",
  "iti64:3.4_0": "yañcaññaṁ dosasaṁhitaṁ.",
  "iti64:4.1_0": "Akatvā kusalaṁ kammaṁ,",
  "iti64:4.2_0": "katvānākusalaṁ bahuṁ;",
  "iti64:4.3_0": "Kāyassa bhedā duppañño,",
  "iti64:4.4_0": "nirayaṁ sopapajjatī”ti.",
  "iti64:5.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti64:5.2_0": "Pañcamaṁ."
}