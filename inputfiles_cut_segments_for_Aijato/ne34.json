{
  "ne34:0.1_0": "Netti",
  "ne34:0.2_0": "Paṭiniddesavāra",
  "ne34:0.3_0": "Sampāta 15",
  "ne34:0.4_0": "Parikkhārahārasampāta",
  "ne34:1.1_0": "Tattha katamo parikkhāro hārasampāto?",
  "ne34:2.1_0": "“Tasmā rakkhitacittassa, sammāsaṅkappagocaro”ti gāthā.",
  "ne34:2.2_0": "Ayaṁ samathavipassanāya parikkhāro.",
  "ne34:2.3_0": "Niyutto parikkhāro hārasampāto."
}