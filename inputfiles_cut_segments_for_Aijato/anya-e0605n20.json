{
  "anya-e0605n20:0_0": "Byāpetādiyamakagāthā",
  "anya-e0605n20:1_0": "110.",
  "anya-e0605n20:2_0": " Ekantameva saparatthaparo mahesi,",
  "anya-e0605n20:3_0": "Ekantameva dasapāramitābalena;",
  "anya-e0605n20:4_0": "Ekantameva hatamārabalena tena,",
  "anya-e0605n20:5_0": "Ekantameva suvisuddhamalattha bodhiṁ."
}