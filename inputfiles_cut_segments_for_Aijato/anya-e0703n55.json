{
  "anya-e0703n55:0_0": "Chapaññāsatima pariccheda",
  "anya-e0703n55:1_0": "Anurādhapurābhigamano",
  "anya-e0703n55:2_0": "1.",
  "anya-e0703n55:3_0": "Yuvarājapade tassa, ṭhitassā’tha nayaññuno;",
  "anya-e0703n55:4_0": "Ahū vijayabāhūti, nāmaṁ sabbattha pākaṭaṁ.",
  "anya-e0703n55:5_0": "2.",
  "anya-e0703n55:6_0": "Mahāññaṇo nijāṇāya, tattha bheriṁ carāviya;",
  "anya-e0703n55:7_0": "Ṭhapento sacive’neke, patirūpe padantare.",
  "anya-e0703n55:8_0": "3.",
  "anya-e0703n55:9_0": "Coḷānaṁ maddanatthāya, rājaraṭṭhādhivāsinaṁ;",
  "anya-e0703n55:10_0": "Caturo caturopāye, yo jayaṁ tattha so vasī.",
  "anya-e0703n55:11_0": "4.",
  "anya-e0703n55:12_0": "Coḷarājā suṇitvā taṁ, puḷatthinagare ṭhitaṁ;",
  "anya-e0703n55:13_0": "Senāpatima pesesi, datvāna balavāhanaṁ.",
  "anya-e0703n55:14_0": "5.",
  "anya-e0703n55:15_0": "Senindaṁ kājaraggāma-samīpaṁ samupāgataṁ;",
  "anya-e0703n55:16_0": "Duppasayhaṁ viditvāna, giriduggamagā tato.",
  "anya-e0703n55:17_0": "6.",
  "anya-e0703n55:18_0": "Senindo kājaraggāmaṁ, vilumpitvāna vegasā;",
  "anya-e0703n55:19_0": "Tattha vatthumasakkonto, sadesaṁ puna rāgami.",
  "anya-e0703n55:20_0": "7.",
  "anya-e0703n55:21_0": "Tato mahādipādo’pi, āgamma malayā lahuṁ;",
  "anya-e0703n55:22_0": "Mahatā balakāyena, sippatthalakamāvasi.",
  "anya-e0703n55:23_0": "8.",
  "anya-e0703n55:24_0": "Rājā rāmaññavisaye, rājino santikaṁ jane;",
  "anya-e0703n55:25_0": "Pesesi bahukesāraṁ, dhanajātañca’nappakaṁ.",
  "anya-e0703n55:26_0": "9.",
  "anya-e0703n55:27_0": "Vicittavatthakappūra-candanādīhi vatthūhi;",
  "anya-e0703n55:28_0": "Paripuṇṇā ca nāvāyo, nekātitthe samosaṭā.",
  "anya-e0703n55:29_0": "10.",
  "anya-e0703n55:30_0": "Dhanajā tehi nekehi, karonto bhaṭasaṅgahaṁ;",
  "anya-e0703n55:31_0": "Tato tambalagāmamhi, nivasittha mahabbalo.",
  "anya-e0703n55:32_0": "11.",
  "anya-e0703n55:33_0": "Aññamaññaviruddhattā, rājāraṭṭhādhivāsino;",
  "anya-e0703n55:34_0": "Upaggamma janā sabbe, karaṁ no denti sabbaso.",
  "anya-e0703n55:35_0": "12.",
  "anya-e0703n55:36_0": "Vipakkhācoḷarājassa, bhindantā’ṇaṁ maduddhatā;",
  "anya-e0703n55:37_0": "Āyuttake vihiṁsento, yathākāmaṁ caranti ca.",
  "anya-e0703n55:38_0": "13.",
  "anya-e0703n55:39_0": "Taṁ sutvā coḷabhūpālo, sampakopavasīkato;",
  "anya-e0703n55:40_0": "Senaṁ mahantiṁ datvāna, pesesi sacivaṁ nijaṁ.",
  "anya-e0703n55:41_0": "14.",
  "anya-e0703n55:42_0": "Mahātitthe’vahiṇṇo so, tattha tattha bahūjane;",
  "anya-e0703n55:43_0": "Ghātento savase katvā, rājāraṭṭhādhivāsino.",
  "anya-e0703n55:44_0": "15.",
  "anya-e0703n55:45_0": "Anukkamena gantvāna, kharāṇo rohaṇaṁ tadā;",
  "anya-e0703n55:46_0": "Ajjhotthari saseno sa, bhinno velovasāgaro.",
  "anya-e0703n55:47_0": "16.",
  "anya-e0703n55:48_0": "Rañño paccatthikā hutvā, ravidevacalavhayā;",
  "anya-e0703n55:49_0": "Ubho damiḷaseninda-vasaṁ yātā mahabbalā.",
  "anya-e0703n55:50_0": "17.",
  "anya-e0703n55:51_0": "Mahāpakkhabalopete, te passiya camūpati;",
  "anya-e0703n55:52_0": "Rohaṇaṁ sakalaṁ khippaṁ, maññittha sakahatthagaṁ.",
  "anya-e0703n55:53_0": "18.",
  "anya-e0703n55:54_0": "Ekādasamhi so vasse, rājā coḷābhibhūtiyā;",
  "anya-e0703n55:55_0": "Paluṭṭhapabbate duggaṁ, pavidhāya tahiṁ vasī.",
  "anya-e0703n55:56_0": "19.",
  "anya-e0703n55:57_0": "Coḷasenā tadāselaṁ, samantā samparikkhipi;",
  "anya-e0703n55:58_0": "Tattho’bhayesaṁ senāna-mahū bhiṁsanakaṁ raṇaṁ.",
  "anya-e0703n55:59_0": "20.",
  "anya-e0703n55:60_0": "Rañño bhaṭā vināsetvā, sabbaṁ taṁ damiḷaṁ balaṁ;",
  "anya-e0703n55:61_0": "Palāyantaṁ mahācoḷa-sāmantañcānubandhiya.",
  "anya-e0703n55:62_0": "21.",
  "anya-e0703n55:63_0": "Gahetvāna siraṁ tassa, gāmasmiṁ tambaviṭṭhike",
  "anya-e0703n55:64_0": "Saddhiṁ vāhanayānehi, sārabhūtadhanehi ca.",
  "anya-e0703n55:65_0": "22.",
  "anya-e0703n55:66_0": "Tahiṁ taṁ sakalaṁ bhaṇḍaṁ, netvā rañño padassiya;",
  "anya-e0703n55:67_0": "Puḷatthinagaraṁ gantuṁ, kālo iti samabravuṁ.",
  "anya-e0703n55:68_0": "23.",
  "anya-e0703n55:69_0": "Mahīpālopi taṁ sutvā, sacivānaṁ vaco tadā;",
  "anya-e0703n55:70_0": "Mahatā balakāyena, pulatthinagaraṁ gato.",
  "anya-e0703n55:71_0": "24.",
  "anya-e0703n55:72_0": "Pavattimetaṁ sakalaṁ, sutvā coḷanarissaro;",
  "anya-e0703n55:73_0": "Tibbakopavasaṁyāto, bhūpālagahaṇatthiko.",
  "anya-e0703n55:74_0": "25.",
  "anya-e0703n55:75_0": "Sāmaṁ khippaṁ samāgamma, vīro sāgarapaṭṭanaṁ;",
  "anya-e0703n55:76_0": "Bhiyyopi mahatiṁ senaṁ, laṁkādīpamapesayī.",
  "anya-e0703n55:77_0": "26.",
  "anya-e0703n55:78_0": "Taṁ vijāniya bhūmindo, senindaṁ pesayī sakaṁ;",
  "anya-e0703n55:79_0": "Balaṁ mahantaṁ datvāna, coḷasenāya yujjhituṁ.",
  "anya-e0703n55:80_0": "27.",
  "anya-e0703n55:81_0": "Gacchamāno camūnātho, so’nurādhapurantike;",
  "anya-e0703n55:82_0": "Saddhiṁ damiḷasenāya, vattesi tumulaṁ raṇaṁ.",
  "anya-e0703n55:83_0": "28.",
  "anya-e0703n55:84_0": "Patiṁsu tasmiṁ saṅgāme, bhūpālassa narā bahū;",
  "anya-e0703n55:85_0": "Bhiyyopi damiḷāyattā, jātā taṁ raṭṭhavāsino.",
  "anya-e0703n55:86_0": "29.",
  "anya-e0703n55:87_0": "Vihāya dharaṇīpālo, pulatthinagaraṁ tadā;",
  "anya-e0703n55:88_0": "Villikābāṇakaṁ raṭṭhaṁ, sampāpuṇiya vegasā.",
  "anya-e0703n55:89_0": "30.",
  "anya-e0703n55:90_0": "Nihaccāmaccayugalaṁ, taṁ raṭṭhādhiṭṭhitaṁ tadā;",
  "anya-e0703n55:91_0": "Tahiṁ vāsamakappesi, sabhaṭe sannipātayaṁ.",
  "anya-e0703n55:92_0": "31.",
  "anya-e0703n55:93_0": "Attānamanubandhantaṁ, sutvā coḷacamūpatiṁ;",
  "anya-e0703n55:94_0": "Gantvā vātagiriṁ nāma, samayaññū siluccayaṁ.",
  "anya-e0703n55:95_0": "32.",
  "anya-e0703n55:96_0": "Upaccakāya selassa, tattha duggaṁ nivesiya;",
  "anya-e0703n55:97_0": "Raṇaṁ karonto temāsaṁ, damiḷe paṭibāhayi.",
  "anya-e0703n55:98_0": "33.",
  "anya-e0703n55:99_0": "Kesadhātukanāthassa, māritassa purāraṇe;",
  "anya-e0703n55:100_0": "Bhātā kaṇiṭṭho sampatta, mahāpakkhabalo tadā.",
  "anya-e0703n55:101_0": "34.",
  "anya-e0703n55:102_0": "Māraṇaṁ sakabhātussa, saranto ruṭṭhamānaso;",
  "anya-e0703n55:103_0": "Sakalaṁ parivattesi, guttahālakamaṇḍalaṁ.",
  "anya-e0703n55:104_0": "35.",
  "anya-e0703n55:105_0": "Atho laṁkissaro tattha, khippaṁ gantvā mahabbalo;",
  "anya-e0703n55:106_0": "Ṭhāne maccutthale nāma, khandhāvāraṁ nibandhiya.",
  "anya-e0703n55:107_0": "36.",
  "anya-e0703n55:108_0": "Khadiraṅgaṇiduggamhā, palāpetvāna taṁ raṇe;",
  "anya-e0703n55:109_0": "Ṭhānā kubulagallā ca, yujjhanto taṁ palāpayi.",
  "anya-e0703n55:110_0": "37.",
  "anya-e0703n55:111_0": "Vihāya puttadārādi-sabbaṁ nekadhanaṁ balaṁ;",
  "anya-e0703n55:112_0": "Palāyamāno so raṭṭhaṁ, coḷāyatta magālahuṁ.",
  "anya-e0703n55:113_0": "38.",
  "anya-e0703n55:114_0": "Tadā narissaro tata, tassa sabbaṁ samādiya;",
  "anya-e0703n55:115_0": "Gantvā tambalagāmamhi, navaṁ duggaṁ nivesiya.",
  "anya-e0703n55:116_0": "39.",
  "anya-e0703n55:117_0": "Anukkamena gantvāna, mahānāgahulavhaye;",
  "anya-e0703n55:118_0": "Purevasī susajjento, balaṁ coḷehi yujjhituṁ.",
  "anya-e0703n55:119_0": "40.",
  "anya-e0703n55:120_0": "Tato rājā samavhāya, sacīve dve mahabbale;",
  "anya-e0703n55:121_0": "Pesesi dikkhiṇaṁ passaṁ, vasaṁ netuṁ tahiṁ jane.",
  "anya-e0703n55:122_0": "41.",
  "anya-e0703n55:123_0": "Sampesesi mahāmacca-yugalaṁ kakkhalaṁ vibhū;",
  "anya-e0703n55:124_0": "Coḷadappavināsāya, tato velā mahāpathe.",
  "anya-e0703n55:125_0": "42.",
  "anya-e0703n55:126_0": "Pesitā dakkhiṇaṁ passaṁ, amaccā’me mahabbalā;",
  "anya-e0703n55:127_0": "Muhunnaruggamaduggaṁ, badaḷatthalameva ca.",
  "anya-e0703n55:128_0": "43.",
  "anya-e0703n55:129_0": "Vāpīnagaraduggañca , buddhagāmakameva ca;",
  "anya-e0703n55:130_0": "Tilagullaṁ mahāgallaṁ, maṇḍagallakameva ca.",
  "anya-e0703n55:131_0": "44.",
  "anya-e0703n55:132_0": "Anurādhapurañcāti, gahetvāna kamena te;",
  "anya-e0703n55:133_0": "Vattentā savase raṭṭhaṁ, mahātitthamupāgatā.",
  "anya-e0703n55:134_0": "45.",
  "anya-e0703n55:135_0": "Pesitā sacivādvetu, tato velā mahāpathe;",
  "anya-e0703n55:136_0": "Vilumpantā chagāmādi-khandhāvāre tahiṁ tahiṁ.",
  "anya-e0703n55:137_0": "46.",
  "anya-e0703n55:138_0": "Pulatthinagarāsannaṁ, kamenā’gamma pesayuṁ;",
  "anya-e0703n55:139_0": "Dūte rājantikakhippa-māgantuṁ vaṭṭatītiha.",
  "anya-e0703n55:140_0": "47.",
  "anya-e0703n55:141_0": "Disāsā dvīsu yānehi, sacivehi pavattitaṁ;",
  "anya-e0703n55:142_0": "Vikkamāti sayaṁ sutvā, kālaññū so mahīpahi.",
  "anya-e0703n55:143_0": "48.",
  "anya-e0703n55:144_0": "Sīghaṁ sannayha senaṅgaṁ, samaggaṁ vidikovido;",
  "anya-e0703n55:145_0": "Ummūlanāyacoḷānaṁ, purātamhā’bhinikkhami.",
  "anya-e0703n55:146_0": "49.",
  "anya-e0703n55:147_0": "Gacchaṁ gaṅgāya mahiya-ṅgaṇathūpantike vibhū;",
  "anya-e0703n55:148_0": "Senānivesaṁ kāretvā kañcikālaṁ tahiṁ vasī.",
  "anya-e0703n55:149_0": "50.",
  "anya-e0703n55:150_0": "Kamenāgamma ṭhānaññū, puḷatthinagarantike;",
  "anya-e0703n55:151_0": "Bandhāpesi mahāvīro, khandhāvāraṁ thiraṁ varaṁ.",
  "anya-e0703n55:152_0": "51.",
  "anya-e0703n55:153_0": "Tattha tattha ṭhitā sūrā, coḷā ye kakkhalā’khilā;",
  "anya-e0703n55:154_0": "Pulatthinagare yuddhaṁ, kātuṁ sannipatiṁsu te.",
  "anya-e0703n55:155_0": "52.",
  "anya-e0703n55:156_0": "Nikkhamma nagarā gantvā, coḷā bahi mahāraṇaṁ;",
  "anya-e0703n55:157_0": "Katvā parājitā bhītā, pavissa nagaraṁ sakaṁ.",
  "anya-e0703n55:158_0": "53.",
  "anya-e0703n55:159_0": "Guttā sesapuradvārā, gopuraṭṭālanissitā;",
  "anya-e0703n55:160_0": "Mahāhavaṁ bahussāhā, pavattesuṁ bhayāvahaṁ.",
  "anya-e0703n55:161_0": "54.",
  "anya-e0703n55:162_0": "Diyaḍḍhamāsaṁ yujjhanti, nagaraṁ tamuparundhiya;",
  "anya-e0703n55:163_0": "Sādhetuṁ nevasā’sakkhi, bhūpālassa mahācamū.",
  "anya-e0703n55:164_0": "55.",
  "anya-e0703n55:165_0": "Mahārañño mahāvīrā, mahāsūrā mahabbalā;",
  "anya-e0703n55:166_0": "Mahābhaṭā mahāmānā, ravidevacalādayo.",
  "anya-e0703n55:167_0": "56.",
  "anya-e0703n55:168_0": "Ullaṅghitvāna pākāraṁ, pavissa sahasā puraṁ;",
  "anya-e0703n55:169_0": "Khaṇena damiḷe sabbe, mūlaghaccamaghātayuṁ.",
  "anya-e0703n55:170_0": "57.",
  "anya-e0703n55:171_0": "Evaṁ laddhajayo rājā, tadā vijayabāhuso;",
  "anya-e0703n55:172_0": "Carāpesi nijāṇāya, bheriṁ bhūrimatipure.",
  "anya-e0703n55:173_0": "58.",
  "anya-e0703n55:174_0": "Taṁ sutvā sakasenāya, vināsaṁ coḷabhūpati;",
  "anya-e0703n55:175_0": "Sīhaḷā balavantoti, bhiyyo senaṁ na pāhiṇi.",
  "anya-e0703n55:176_0": "59.",
  "anya-e0703n55:177_0": "Vīro asesanihatuddhaṭa coḷaseṭṭho;",
  "anya-e0703n55:178_0": "Viññū susādhuṭṭhapitākhilarājaraṭṭho;",
  "anya-e0703n55:179_0": "Iṭṭhannurādhapuraseṭṭhamatīva haṭṭho;",
  "anya-e0703n55:180_0": "Vassamhi pañcadasame gami rājaseṭṭho.",
  "anya-e0703n55:181_0": "Sujanappasādasaṁvegatthāya kate mahāvaṁse",
  "anya-e0703n55:182_0": "Anurādhapurābhigamano nāma",
  "anya-e0703n55:183_0": "Chappaññāsatimo paricchedo"
}