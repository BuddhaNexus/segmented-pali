{
  "anya-e1009n1:0_0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1009n1:1_0": "Naradakkhadīpanī",
  "anya-e1009n1:2_0": "Pālito yeva saddhammo,",
  "anya-e1009n1:3_0": "Pālitena varena ca;",
  "anya-e1009n1:4_0": "Pālite suṭṭhu yaṁ sīlaṁ,",
  "anya-e1009n1:5_0": "Pālitaṁ dhamma-sundaraṁ.",
  "anya-e1009n1:6_0": "Sammā āraddhaṁ sabba-sampattīnaṁ,",
  "anya-e1009n1:7_0": "Mūlaṁ hotīti daṭṭhabbaṁ.",
  "anya-e1009n1:8_0": "Vīriyārabbho bhikkhave,",
  "anya-e1009n1:9_0": "Mahato atthāya saṁvattatīti.",
  "anya-e1009n1:10_0": "Tasmā vīriyameva kattabbaṁ,",
  "anya-e1009n1:11_0": "Vīriyavato hi acintitaṁpi hoti.",
  "anya-e1009n1:12_0": "Yathā hi tacchakānaṁ suttaṁ,",
  "anya-e1009n1:13_0": "Pamāṇaṁ hoti;",
  "anya-e1009n1:14_0": "Eva metampi viññūnaṁ.",
  "anya-e1009n1:15_0": "Suvijāno bhavaṁ hoti,",
  "anya-e1009n1:16_0": "Dubbijāno parābhavo;",
  "anya-e1009n1:17_0": "Dhammakāmo bhavaṁ hoti,",
  "anya-e1009n1:18_0": "Dhammadessī parābhavo.",
  "anya-e1009n1:19_0": "Ayaṁ dhammatāti ayaṁ sabhāvo,",
  "anya-e1009n1:20_0": "Ayaṁ niyāmoti vuttaṁ hoti.",
  "anya-e1009n1:21_0": "Ye dhammā hetupabhavā,",
  "anya-e1009n1:22_0": "Tesaṁ hetuṁ tathāgato.",
  "anya-e1009n1:23_0": "Ratanattayaṁ , santataṁ, ahaṁ vandāmi;",
  "anya-e1009n1:24_0": "Ācariyaṁ, so ahaṁ, niccaṁ namāmi;",
  "anya-e1009n1:25_0": "‘‘Hotu sabbaṁ, maṅgalaṁ, mamaṁ sabbadhi’’.",
  "anya-e1009n1:26_0": "Naradakkhadīpanī",
  "anya-e1009n1:27_0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1009n1:28_0": "Paṇāma"
}