{
  "tika-abh06t72:0.1_0": "Saccasaṅkhepo",
  "tika-abh06t72:0.2_0": "Ganthārambhakathā",
  "tika-abh06t72:1_0": "1.",
  "tika-abh06t72:2_0": "Namassitvā tilokaggaṁ, ñeyyasāgarapāraguṁ;",
  "tika-abh06t72:3_0": "Bhavābhāvakaraṁ dhammaṁ, gaṇañca guṇasāgaraṁ.",
  "tika-abh06t72:4_0": "2.",
  "tika-abh06t72:5_0": "Nissāya pubbācariyamataṁ atthāvirodhinaṁ;",
  "tika-abh06t72:6_0": "Vakkhāmi saccasaṅkhepaṁ, hitaṁ kārakayoginaṁ."
}