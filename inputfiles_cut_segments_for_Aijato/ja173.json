{
  "ja173:0.1_0": "Jātaka",
  "ja173:0.2_0": "Dukanipāta",
  "ja173:0.3_0": "Kalyāṇavagga",
  "ja173:0.4_0": "3. Makkaṭajātaka",
  "ja173:1.1_0": "“Tāta māṇavako eso,",
  "ja173:1.2_0": "tālamūlaṁ apassito;",
  "ja173:1.3_0": "Agārakañcidaṁ atthi,",
  "ja173:1.4_0": "handa demassagārakaṁ”.",
  "ja173:2.1_0": "“Mā kho tvaṁ tāta pakkosi,",
  "ja173:2.2_0": "dūseyya no agārakaṁ;",
  "ja173:2.3_0": "Netādisaṁ mukhaṁ hoti,",
  "ja173:2.4_0": "brāhmaṇassa susīlino”ti.",
  "ja173:3.1_0": "Makkaṭajātakaṁ tatiyaṁ."
}