{
  "ja303:0.1_0": "Jātaka",
  "ja303:0.2_0": "Catukkanipāta",
  "ja303:0.3_0": "Kāliṅgavagga",
  "ja303:0.4_0": "3. Ekarājajātaka",
  "ja303:1.1_0": "“Anuttare kāmaguṇe samiddhe,",
  "ja303:1.2_0": "Bhutvāna pubbe vasī ekarāja;",
  "ja303:1.3_0": "So dāni dugge narakamhi khitto,",
  "ja303:1.4_0": "Nappajjahe vaṇṇabalaṁ purāṇaṁ”.",
  "ja303:2.1_0": "“Pubbeva khantī ca tapo ca mayhaṁ,",
  "ja303:2.2_0": "Sampatthitā dubbhisena ahosi;",
  "ja303:2.3_0": "Taṁ dāni laddhāna kathaṁ nu rāja,",
  "ja303:2.4_0": "Jahe ahaṁ vaṇṇabalaṁ purāṇaṁ.",
  "ja303:3.1_0": "Sabbā kirevaṁ pariniṭṭhitāni,",
  "ja303:3.2_0": "Yasassinaṁ paññavantaṁ visayha;",
  "ja303:3.3_0": "Yaso ca laddhā purimaṁ uḷāraṁ,",
  "ja303:3.4_0": "Nappajjahe vaṇṇabalaṁ purāṇaṁ.",
  "ja303:4.1_0": "Panujja dukkhena sukhaṁ janinda,",
  "ja303:4.2_0": "Sukhena vā dukkhamasayhasāhi;",
  "ja303:4.3_0": "Ubhayattha santo abhinibbutattā,",
  "ja303:4.4_0": "Sukhe ca dukkhe ca bhavanti tulyā”ti.",
  "ja303:5.1_0": "Ekarājajātakaṁ tatiyaṁ."
}