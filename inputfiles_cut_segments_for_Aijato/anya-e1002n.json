{
  "anya-e1002n:0_0": " Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1002n:1_0": "Nītimañjarī",
  "anya-e1002n:2_0": "1.",
  "anya-e1002n:3_0": " Kulakkhaye vinassanti,",
  "anya-e1002n:4_0": "Kuladhammā sanantanā;",
  "anya-e1002n:5_0": "Dhamme naṭṭhe kulaṁ sabbaṁ,",
  "anya-e1002n:6_0": "Adhammo abhibhū khalaṁ.",
  "anya-e1002n:7_0": "2.",
  "anya-e1002n:8_0": " Adhammābhibhavā dantā,",
  "anya-e1002n:9_0": "Padussanti kulitthiyo;",
  "anya-e1002n:10_0": "Thīsu duṭṭhā sva dhammena,",
  "anya-e1002n:11_0": "Jāyate vaṇṇasaṅkaro.",
  "anya-e1002n:12_0": "3.",
  "anya-e1002n:13_0": " Piyaṁ bhāse guṇaggāho,",
  "anya-e1002n:14_0": "Sūro siyā vikantano;",
  "anya-e1002n:15_0": "Dātā candasamā nārī,",
  "anya-e1002n:16_0": "Diṭṭhaṁ diṭṭhaṁ nahāsaye.",
  "anya-e1002n:17_0": "4.",
  "anya-e1002n:18_0": " Kutotthi kumitte saccaṁ,",
  "anya-e1002n:19_0": "Kudāre rativaḍḍhanaṁ;",
  "anya-e1002n:20_0": "Kudesamhi mano rammaṁ,",
  "anya-e1002n:21_0": "Kurāje bhogasampadaṁ.",
  "anya-e1002n:22_0": "Saṅketeva amittasmiṁ,",
  "anya-e1002n:23_0": "Mittasmiṁ pi navissase;",
  "anya-e1002n:24_0": "Abhayā bhaya muppannaṁ,",
  "anya-e1002n:25_0": "Api mūlāni kantati.",
  "anya-e1002n:26_0": "Adiṭṭhova paro seyyo,",
  "anya-e1002n:27_0": "Dummitto no vissāsiko.",
  "anya-e1002n:28_0": "Aggihomaphalaṁ vedo,",
  "anya-e1002n:29_0": "Satthaṁsīlaphalaṁ mataṁ;",
  "anya-e1002n:30_0": "Ratiputtaphalaṁ nārī,",
  "anya-e1002n:31_0": "Dānabhuttiphalaṁ dhanaṁ.",
  "anya-e1002n:32_0": "Asaccaṁ sāhasaṁ māyā,",
  "anya-e1002n:33_0": "Mūḷhatta ma tilobhatā;",
  "anya-e1002n:34_0": "Asocaṁ niddayattañca,",
  "anya-e1002n:35_0": "Thīnaṁ dosā sabhāvajā.",
  "anya-e1002n:36_0": "Jāyāya bhattuno bhāro,",
  "anya-e1002n:37_0": "Sissena guruno kato;",
  "anya-e1002n:38_0": "Amaccakehi rājassa,",
  "anya-e1002n:39_0": "Pitarānaṁ nijenaca.",
  "anya-e1002n:40_0": "5.",
  "anya-e1002n:41_0": " Uyyamena hi sijjhanti,",
  "anya-e1002n:42_0": "Kammāni na manorathā;",
  "anya-e1002n:43_0": "Na hi suttassa sīhassa,",
  "anya-e1002n:44_0": "Pavīsanti mukhe migā.",
  "anya-e1002n:45_0": "Atisītaṁ atiuṇhaṁ,",
  "anya-e1002n:46_0": "Atisāyamidaṁ ahu;",
  "anya-e1002n:47_0": "Iti visaṭṭhakammante,",
  "anya-e1002n:48_0": "Khaṇā accenti māṇave.",
  "anya-e1002n:49_0": "Ādānassa padānassa,",
  "anya-e1002n:50_0": "Kattabbassa ca kammuno;",
  "anya-e1002n:51_0": "Khippaṁ akayyamānassa,",
  "anya-e1002n:52_0": "Kāle pivati sampadaṁ.",
  "anya-e1002n:53_0": "Nādabbe nihitā kāci,",
  "anya-e1002n:54_0": "Kriyā phalavatī bhave;",
  "anya-e1002n:55_0": "Nabyāpārasatenāpi,",
  "anya-e1002n:56_0": "Sukova pāṭhate bako.",
  "anya-e1002n:57_0": "Yo dandhakāle tarati,",
  "anya-e1002n:58_0": "Taraṇīye ca dandhaye;",
  "anya-e1002n:59_0": "Sukkhapaṇṇaṁ va akkamma,",
  "anya-e1002n:60_0": "Atthaṁ bhañjati attano.",
  "anya-e1002n:61_0": "6.",
  "anya-e1002n:62_0": " Yaṁ dadāti yaṁ bhuñjati,",
  "anya-e1002n:63_0": "Tadeva dhanino dhanaṁ;",
  "anya-e1002n:64_0": "Aññe matassa kīḷanti,",
  "anya-e1002n:65_0": "Dārehipi dhanehipi.",
  "anya-e1002n:66_0": "Dānopabhogahīnena ,",
  "anya-e1002n:67_0": "Dhanena dhanino sukhaṁ;",
  "anya-e1002n:68_0": "Ko viseso daliddassa,",
  "anya-e1002n:69_0": "Adhikaṁ dhanarakkhaṇaṁ.",
  "anya-e1002n:70_0": "Nijasokhyaṁ nirundhanto,",
  "anya-e1002n:71_0": "Nīcabhogo mitampaco;",
  "anya-e1002n:72_0": "Dhanaṁ sañcayate yo so,",
  "anya-e1002n:73_0": "Parabhāravaho pasu.",
  "anya-e1002n:74_0": "Yaṁ ussukā saṅkharonti,",
  "anya-e1002n:75_0": "Alakkhikā bahuṁ dhanaṁ;",
  "anya-e1002n:76_0": "Sippavanto asippāvā,",
  "anya-e1002n:77_0": "Lakkhi vā tāni bhuñjati.",
  "anya-e1002n:78_0": "7.",
  "anya-e1002n:79_0": " Sampatyaṁ mahataṁ cittaṁ,",
  "anya-e1002n:80_0": "Bhave uppale komalaṁ;",
  "anya-e1002n:81_0": "Vipatyaṁca mahāsela,",
  "anya-e1002n:82_0": "Silāsaṅghātakakkasaṁ.",
  "anya-e1002n:83_0": "8.",
  "anya-e1002n:84_0": " Asambhabyaguṇaṁ thutvā,",
  "anya-e1002n:85_0": "Khedo mudhāva jāyate;",
  "anya-e1002n:86_0": "Avhāyaṁ canda mu llokya,",
  "anya-e1002n:87_0": "Nacandota mu pāgamī.",
  "anya-e1002n:88_0": "9.",
  "anya-e1002n:89_0": " Saccaṁ mukhamhi dhāreyya,",
  "anya-e1002n:90_0": "Kaṇṇe sutaṁ bhuje jayaṁ;",
  "anya-e1002n:91_0": "Hadayamhi khamaṁ vīraṁ,",
  "anya-e1002n:92_0": "Lokādāsaṁca locane.",
  "anya-e1002n:93_0": "Saddamattaṁ naphandeyya,",
  "anya-e1002n:94_0": "Aññatvā saddakāraṇaṁ;",
  "anya-e1002n:95_0": "Saddahetuṁ pariññāya,",
  "anya-e1002n:96_0": "Pamodo vā bhayo tathā.",
  "anya-e1002n:97_0": "Sabbasuta ma dhīyeyya,",
  "anya-e1002n:98_0": "Hīnamukkaṭṭhamajjhimaṁ.",
  "anya-e1002n:99_0": "10.",
  "anya-e1002n:100_0": " Dunnāriyā kulaṁ suddhaṁ,",
  "anya-e1002n:101_0": "Putto nassati lālanā;",
  "anya-e1002n:102_0": "Samiddhi anayā bandhu,",
  "anya-e1002n:103_0": "Pavāsā madanā hirī.",
  "anya-e1002n:104_0": "Lālaye pañcavassāni,",
  "anya-e1002n:105_0": "Dasavassāni tālaye;",
  "anya-e1002n:106_0": "Pattetu soḷasevasse,",
  "anya-e1002n:107_0": "Puttaṁ mittaṁva ācare.",
  "anya-e1002n:108_0": "Lālane bahavo dosā,",
  "anya-e1002n:109_0": "Lālane bahavo guṇā.",
  "anya-e1002n:110_0": "Pāpā nivārayati yojayate hitāya,",
  "anya-e1002n:111_0": "Guyhāni gūhati guṇaṁ pakaṭīkaroti;",
  "anya-e1002n:112_0": "Āpattikañca najahāti dadāti kāle,",
  "anya-e1002n:113_0": "Sammitta lakkhaṇamidaṁ pavadanti santo.",
  "anya-e1002n:114_0": "11.",
  "anya-e1002n:115_0": " Dujjano jīyate yutyā,",
  "anya-e1002n:116_0": "Niggahena nadhīmatā;",
  "anya-e1002n:117_0": "Nipātyate mahārukkho,",
  "anya-e1002n:118_0": "Tassamīpa khatikkhayā.",
  "anya-e1002n:119_0": "Vane migāca luddhānaṁ,",
  "anya-e1002n:120_0": "Dujjanānañca sajjanā;",
  "anya-e1002n:121_0": "Akāraṇaverī honti,",
  "anya-e1002n:122_0": "Tiṇabhakkhā supesalā.",
  "anya-e1002n:123_0": "Pādalaggaṁ karaṭṭhena,",
  "anya-e1002n:124_0": "Kaṇḍakeneva kaṇḍakaṁ.",
  "anya-e1002n:125_0": "Bālaṁ napasse nasuṇe,",
  "anya-e1002n:126_0": "Nacabālena saṁvase;",
  "anya-e1002n:127_0": "Bālenāllāpasallāpaṁ,",
  "anya-e1002n:128_0": "Nakare nacarocaye.",
  "anya-e1002n:129_0": "12.",
  "anya-e1002n:130_0": " Upa kattuṁ yathā khuddo,",
  "anya-e1002n:131_0": "Samattho natathāmahā;",
  "anya-e1002n:132_0": "Kūpo hi hanti pipāsaṁ,",
  "anya-e1002n:133_0": "Natu pāyo mahambudhi.",
  "anya-e1002n:134_0": "13.",
  "anya-e1002n:135_0": " Ādānassa padānassa,",
  "anya-e1002n:136_0": "Kattabbassaca kammuno;",
  "anya-e1002n:137_0": "Khippaṁ akaramānassa,",
  "anya-e1002n:138_0": "Kālo bhakkhati taṁ rasaṁ.",
  "anya-e1002n:139_0": "Nakkhattaṁ paṭimānentaṁ,",
  "anya-e1002n:140_0": "Attho bālaṁ upajjhagā;",
  "anya-e1002n:141_0": "Attho atthassa nakkhattaṁ,",
  "anya-e1002n:142_0": "Kiṁ karissanti tārakā.",
  "anya-e1002n:143_0": "Ajarāmarova pañño,",
  "anya-e1002n:144_0": "Vijjamatthañca cintaye;",
  "anya-e1002n:145_0": "Gahitoviya kesesu,",
  "anya-e1002n:146_0": "Maccunā dhammamācare.",
  "anya-e1002n:147_0": "14.",
  "anya-e1002n:148_0": " Vajjā gurūca mantīca,",
  "anya-e1002n:149_0": "Tayo raṭṭhābhisaṅkhatā;",
  "anya-e1002n:150_0": "Jīvīta dakkha kosānaṁ,",
  "anya-e1002n:151_0": "Vaḍḍhanā nāsanāca te.",
  "anya-e1002n:152_0": "15.",
  "anya-e1002n:153_0": " Thirena kammaṁ vaḍḍhati,",
  "anya-e1002n:154_0": "Athirena turena no;",
  "anya-e1002n:155_0": "Phalanti samaye rukkhā,",
  "anya-e1002n:156_0": "Sittāpi bahuvārinā.",
  "anya-e1002n:157_0": "Vāyāmetheva puriso,",
  "anya-e1002n:158_0": "Nanibbindeyya paṇḍito.",
  "anya-e1002n:159_0": "Payatano tādiso neva,",
  "anya-e1002n:160_0": "Kayyo yena phalaṁ nahi;",
  "anya-e1002n:161_0": "Selagge kūpakhaṇanā,",
  "anya-e1002n:162_0": "Kathaṁ toyasamāgamo.",
  "anya-e1002n:163_0": "Ñāṇaṅkusena sammaggaṁ,",
  "anya-e1002n:164_0": "Niyyatyussāhakuñjaro.",
  "anya-e1002n:165_0": "Asamekkhitakammantaṁ,",
  "anya-e1002n:166_0": "Turitābhi nipātinaṁ;",
  "anya-e1002n:167_0": "Tānikammāni tappenti,",
  "anya-e1002n:168_0": "Uṇhaṁ va jjhohitaṁ mukhe.",
  "anya-e1002n:169_0": "16.",
  "anya-e1002n:170_0": " Chaddosā puriseneha,",
  "anya-e1002n:171_0": "Hātabbā bhūtimicchantā;",
  "anya-e1002n:172_0": "Niddā majjaṁ bhayaṁ kodho,",
  "anya-e1002n:173_0": "Ālasyaṁ dīghasuttatā.",
  "anya-e1002n:174_0": "Na divā suppasīlena,",
  "anya-e1002n:175_0": "Rattimuṭṭhānadessinā;",
  "anya-e1002n:176_0": "Niccasoṇḍena mattena,",
  "anya-e1002n:177_0": "Sakkā āvasituṁ gharaṁ.",
  "anya-e1002n:178_0": "Abhetabbamhi bhāyanti,",
  "anya-e1002n:179_0": "Bhāyitabbe nabhāyare;",
  "anya-e1002n:180_0": "Bhayābhaya vimuḷhā te,",
  "anya-e1002n:181_0": "Jimhānugā ujuñjahā.",
  "anya-e1002n:182_0": "Yassa manussabhūtassa,",
  "anya-e1002n:183_0": "Natthi bhogāca sippakaṁ;",
  "anya-e1002n:184_0": "Kiṁ phalaṁ tassa mānussaṁ,",
  "anya-e1002n:185_0": "Dvipādaṭṭho hi so migo.",
  "anya-e1002n:186_0": "17.",
  "anya-e1002n:187_0": " Nānopāyova kattabbo,",
  "anya-e1002n:188_0": "Sace bhaveyya attano;",
  "anya-e1002n:189_0": "Atthasiddhi yathākāmaṁ,",
  "anya-e1002n:190_0": "Upāyo hi hitañjaso.",
  "anya-e1002n:191_0": "Lañjadānabālisena ,",
  "anya-e1002n:192_0": "Kūṭaḍḍakāradhīvarā;",
  "anya-e1002n:193_0": "Vinicchayamahāmacchaṁ,",
  "anya-e1002n:194_0": "Oṭṭenti lobhasāgare.",
  "anya-e1002n:195_0": "Yassete caturo dhammā,",
  "anya-e1002n:196_0": "Vānarinda yathātava;",
  "anya-e1002n:197_0": "Saccaṁ dhammo dhīti cāgo,",
  "anya-e1002n:198_0": "Diṭṭhaṁ so ativattati.",
  "anya-e1002n:199_0": "18.",
  "anya-e1002n:200_0": " Vidvāca ratanaṁ nārī,",
  "anya-e1002n:201_0": "Vīṇā sātthaṁ giraṁmahī;",
  "anya-e1002n:202_0": "Guṇavisesa māgamma,",
  "anya-e1002n:203_0": "Guṇāni aguṇānica.",
  "anya-e1002n:204_0": "Dhanavā balavā loke,",
  "anya-e1002n:205_0": "Dhanā bhavati paṇḍito.",
  "anya-e1002n:206_0": "Sumane nissito kīṭo,",
  "anya-e1002n:207_0": "Nigguṇo hīnako sayaṁ;",
  "anya-e1002n:208_0": "Taṁ pupphehi maṇḍentānaṁ,",
  "anya-e1002n:209_0": "Raññaṁ siropi rohati.",
  "anya-e1002n:210_0": "Alakkhikehi sañcītā,",
  "anya-e1002n:211_0": "Dhanabhogāca cintitā;",
  "anya-e1002n:212_0": "Lakkhikassa bhavantete,",
  "anya-e1002n:213_0": "Lakkhivā suṭṭhubhuñjati.",
  "anya-e1002n:214_0": "Khattiyo seṭṭho jane tasmiṁ,",
  "anya-e1002n:215_0": "Yo gottapaṭisārino;",
  "anya-e1002n:216_0": "Vijjācaraṇasampanno,",
  "anya-e1002n:217_0": "So seṭṭho devamānuse.",
  "anya-e1002n:218_0": "Visāpi amataṁ gaṇhe,",
  "anya-e1002n:219_0": "Gūthato maṇimuttamaṁ;",
  "anya-e1002n:220_0": "Kaṇṭakapādapā pupphaṁ,",
  "anya-e1002n:221_0": "Thirataṁ dukkulā varaṁ.",
  "anya-e1002n:222_0": "Dhanissarādiguṇommi -",
  "anya-e1002n:223_0": "Vegena vāhitā pajā.",
  "anya-e1002n:224_0": "19.",
  "anya-e1002n:225_0": " Yassa tthi satataṁ mettā,",
  "anya-e1002n:226_0": "Sabbalokasuvallabhā;",
  "anya-e1002n:227_0": "Kūpāyate samuddopi,",
  "anya-e1002n:228_0": "Aggi tassa jalāyate.",
  "anya-e1002n:229_0": "20.",
  "anya-e1002n:230_0": " Sakkharāyati merūpi,",
  "anya-e1002n:231_0": "Visabhakkho sudhāyate;",
  "anya-e1002n:232_0": "Sasāyate migarāja,",
  "anya-e1002n:233_0": "Byālo mālāguṇāyate;",
  "anya-e1002n:234_0": "Dolāyate chamācālo,",
  "anya-e1002n:235_0": "Nānāvudhā tiṇāyare.",
  "anya-e1002n:236_0": "21.",
  "anya-e1002n:237_0": " Sameva sati ussāhe,",
  "anya-e1002n:238_0": "Sukhavāho hitaṅkaro;",
  "anya-e1002n:239_0": "Ūne-dhike tathā nohi,",
  "anya-e1002n:240_0": "Majjhago sādhu sabbadā.",
  "anya-e1002n:241_0": "Sādhu kho paṇḍitonāma,",
  "anya-e1002n:242_0": "Natveva atipaṇḍito."
}