{
  "ja338:0.1_0": "Jātaka",
  "ja338:0.2_0": "Catukkanipāta",
  "ja338:0.3_0": "Kokilavagga",
  "ja338:0.4_0": "8. Thusajātaka",
  "ja338:1.1_0": "“Viditaṁ thusaṁ undurānaṁ,",
  "ja338:1.2_0": "Viditaṁ pana taṇḍulaṁ;",
  "ja338:1.3_0": "Thusaṁ thusaṁ vivajjetvā,",
  "ja338:1.4_0": "Taṇḍulaṁ pana khādare.",
  "ja338:2.1_0": "Yā mantanā araññasmiṁ,",
  "ja338:2.2_0": "yā ca gāme nikaṇṇikā;",
  "ja338:2.3_0": "Yañcetaṁ iti cīti ca,",
  "ja338:2.4_0": "etampi viditaṁ mayā.",
  "ja338:3.1_0": "Dhammena kira jātassa,",
  "ja338:3.2_0": "pitā puttassa makkaṭo;",
  "ja338:3.3_0": "Daharasseva santassa,",
  "ja338:3.4_0": "dantehi phalamacchidā.",
  "ja338:4.1_0": "Yametaṁ parisappasi,",
  "ja338:4.2_0": "ajakāṇova sāsape;",
  "ja338:4.3_0": "Yopāyaṁ heṭṭhato seti,",
  "ja338:4.4_0": "etampi viditaṁ mayā”ti.",
  "ja338:5.1_0": "Thusajātakaṁ aṭṭhamaṁ."
}