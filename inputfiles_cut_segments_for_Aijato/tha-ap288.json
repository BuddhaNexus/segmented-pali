{
  "tha-ap288:0.1_0": "Therāpadāna",
  "tha-ap288:0.2_0": "Paṇṇadāyakavagga",
  "tha-ap288:0.3_0": "6 Upaṭṭhākadāyakattheraapadāna",
  "tha-ap288:1.1_0": "“Rathiyaṁ paṭipajjantaṁ,",
  "tha-ap288:1.2_0": "āhutīnaṁ paṭiggahaṁ;",
  "tha-ap288:1.3_0": "Dvipadindaṁ mahānāgaṁ,",
  "tha-ap288:1.4_0": "lokajeṭṭhaṁ narāsabhaṁ.",
  "tha-ap288:2.1_0": "Pakkosāpiya tassāhaṁ,",
  "tha-ap288:2.2_0": "sabbalokahitesino;",
  "tha-ap288:2.3_0": "Upaṭṭhāko mayā dinno,",
  "tha-ap288:2.4_0": "siddhatthassa mahesino.",
  "tha-ap288:3.1_0": "Paṭiggahetvā sambuddho,",
  "tha-ap288:3.2_0": "niyyādesi mahāmuni;",
  "tha-ap288:3.3_0": "Uṭṭhāya āsanā tamhā,",
  "tha-ap288:3.4_0": "pakkāmi pācināmukho.",
  "tha-ap288:4.1_0": "Catunnavutito kappe,",
  "tha-ap288:4.2_0": "upaṭṭhākamadaṁ tadā;",
  "tha-ap288:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap288:4.4_0": "upaṭṭhānassidaṁ phalaṁ.",
  "tha-ap288:5.1_0": "Sattapaññāsito kappe,",
  "tha-ap288:5.2_0": "balasenasanāmako;",
  "tha-ap288:5.3_0": "Sattaratanasampanno,",
  "tha-ap288:5.4_0": "cakkavattī mahabbalo.",
  "tha-ap288:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap288:6.2_0": "…pe…",
  "tha-ap288:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap288:7.1_0": "Itthaṁ sudaṁ āyasmā upaṭṭhākadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap288:8.1_0": "Upaṭṭhākadāyakattherassāpadānaṁ chaṭṭhaṁ."
}