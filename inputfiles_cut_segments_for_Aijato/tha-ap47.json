{
  "tha-ap47:0.1_0": "Therāpadāna",
  "tha-ap47:0.2_0": "Upālivagga",
  "tha-ap47:0.3_0": "5. Pañcahatthiyattheraapadāna",
  "tha-ap47:1.1_0": "“Sumedho nāma sambuddho,",
  "tha-ap47:1.2_0": "gacchate antarāpaṇe;",
  "tha-ap47:1.3_0": "Okkhittacakkhu mitabhāṇī,",
  "tha-ap47:1.4_0": "satimā saṁvutindriyo.",
  "tha-ap47:2.1_0": "Pañca uppalahatthāni,",
  "tha-ap47:2.2_0": "āveḷatthaṁ ahaṁsu me;",
  "tha-ap47:2.3_0": "Tena buddhaṁ apūjesiṁ,",
  "tha-ap47:2.4_0": "pasanno sehi pāṇibhi.",
  "tha-ap47:3.1_0": "Āropitā ca te pupphā,",
  "tha-ap47:3.2_0": "chadanaṁ assu satthuno;",
  "tha-ap47:3.3_0": "Samādhiṁsu mahānāgaṁ,",
  "tha-ap47:3.4_0": "sissā ācariyaṁ yathā.",
  "tha-ap47:4.1_0": "Tiṁsakappasahassamhi,",
  "tha-ap47:4.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap47:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap47:4.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap47:5.1_0": "Ito vīsakappasate,",
  "tha-ap47:5.2_0": "ahesuṁ pañca khattiyā;",
  "tha-ap47:5.3_0": "Hatthiyā nāma nāmena,",
  "tha-ap47:5.4_0": "cakkavattī mahabbalā.",
  "tha-ap47:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap47:6.2_0": "…pe…",
  "tha-ap47:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap47:7.1_0": "Itthaṁ sudaṁ āyasmā pañcahatthiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap47:8.1_0": "Pañcahatthiyattherassāpadānaṁ pañcamaṁ."
}