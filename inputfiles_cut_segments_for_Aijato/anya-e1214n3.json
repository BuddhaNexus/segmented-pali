{
  "anya-e1214n3:0_0": "Rudhādayo",
  "anya-e1214n3:1_0": "101.",
  "anya-e1214n3:2_0": " Rudhi āvaraṇe muca mocane rica recane,",
  "anya-e1214n3:3_0": "Sica seke yuja yoge bhuja pālanabhojane;",
  "anya-e1214n3:4_0": "102.",
  "anya-e1214n3:5_0": " Katicchede chidi dvedhākaraṇe bhida vidāraṇe",
  "anya-e1214n3:6_0": "Vida lābhe lupacchede vināse lipalimpane",
  "anya-e1214n3:7_0": "Pisa saṁcuṇṇane hisi vihiṁsāyaṁ (rudhādayo;)"
}