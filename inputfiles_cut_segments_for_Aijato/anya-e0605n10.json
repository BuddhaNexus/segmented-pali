{
  "anya-e0605n10:0_0": "Pāduddhāravimhayadīpanīgāthā",
  "anya-e0605n10:1_0": "65.",
  "anya-e0605n10:2_0": " Yāvañcayaṁ ravi caratyacalena ruddhe,",
  "anya-e0605n10:3_0": "Yāvañca cakkaratanañca payāti loke;",
  "anya-e0605n10:4_0": "Tāvissaro nabhacaro jitacāturanto,",
  "anya-e0605n10:5_0": "Hitvā kathaṁ nu padamuddhari so nirāso.",
  "anya-e0605n10:6_0": "66.",
  "anya-e0605n10:7_0": " Dīpe mahā ca caturādhikadvesahasse,",
  "anya-e0605n10:8_0": "Tatrāpi seṭṭhabhajitaṁ varajambudīpaṁ;",
  "anya-e0605n10:9_0": "Bhūnābhikaṁ kapilavatthupuraṁ surammaṁ,",
  "anya-e0605n10:10_0": "Hitvā kathaṁ nu padamuddhari so nirāso.",
  "anya-e0605n10:11_0": "67.",
  "anya-e0605n10:12_0": " Ñātīnasīti kulato hi sahassa sākye,",
  "anya-e0605n10:13_0": "Hatthissadhaññadhanino vijitārisaṇghe;",
  "anya-e0605n10:14_0": "Gottena gotamabhavaṁ pitarañjanaggaṁ,",
  "anya-e0605n10:15_0": "Hitvā kathaṁ nu padamuddhari so nirāso.",
  "anya-e0605n10:16_0": "68.",
  "anya-e0605n10:17_0": " Rammaṁ surammavasatiṁ ratanujjalantaṁ,",
  "anya-e0605n10:18_0": "Gimhepi vimhayakaraṁ suramandirābhaṁ;",
  "anya-e0605n10:19_0": "Ussāpitaddhajapaṭākasitātapattaṁ,",
  "anya-e0605n10:20_0": "Hitvā kathaṁ nu padamuddhari so nirāso.",
  "anya-e0605n10:21_0": "69.",
  "anya-e0605n10:22_0": " Sapokkharā pokkharaṇī catasso,",
  "anya-e0605n10:23_0": "Supupphitā mandirato samantā;",
  "anya-e0605n10:24_0": "Kokā nadantūpari kokanāde,",
  "anya-e0605n10:25_0": "Hitvā kathaṁ nu padamuddhari so nirāso.",
  "anya-e0605n10:26_0": "70.",
  "anya-e0605n10:27_0": " Sare saroje ruditāḷipāḷi,",
  "anya-e0605n10:28_0": "Samantato passati pañjarañjasā;",
  "anya-e0605n10:29_0": "Disvāravindāni mukhāravindaṁ",
  "anya-e0605n10:30_0": "Nāthassa lajjā viya saṁkujanti.",
  "anya-e0605n10:31_0": "71.",
  "anya-e0605n10:32_0": " Madhurā madhurābhirutā,",
  "anya-e0605n10:33_0": "Caritā padume padumeḷigaṇā;",
  "anya-e0605n10:34_0": "Vasatiṁ adhunā madhunā,",
  "anya-e0605n10:35_0": "Akaruṁ jahitaṁ kimidaṁ patinā.",
  "anya-e0605n10:36_0": "72.",
  "anya-e0605n10:37_0": " Tamhā rasaṁ madhukarā bhavanaṁ haritvā,",
  "anya-e0605n10:38_0": "Ninnādino samadhuraṁ madhuraṁ karonti;",
  "anya-e0605n10:39_0": "Nādena nādamatiriccupavīṇayanti,",
  "anya-e0605n10:40_0": "Naccanti tā surapure vaṇitā va tāva.",
  "anya-e0605n10:41_0": "73.",
  "anya-e0605n10:42_0": " Sañcoditā pīṇapayodharādharā,",
  "anya-e0605n10:43_0": "Virājitānaṅgajamekhalākhalā;",
  "anya-e0605n10:44_0": "Suraṅgaṇā vaṅgajaphassadā sadā,",
  "anya-e0605n10:45_0": "Ramā ramāpenti varaṅgadāgadā.",
  "anya-e0605n10:46_0": "74.",
  "anya-e0605n10:47_0": " Karātirattā ratirattarāmā,",
  "anya-e0605n10:48_0": "Tāḷenti tāḷāvacare samantā;",
  "anya-e0605n10:49_0": "Naccuggatānekasahassahatthā,",
  "anya-e0605n10:50_0": "Sakkopi kiṁ sakyasamoti codayuṁ.",
  "anya-e0605n10:51_0": "75.",
  "anya-e0605n10:52_0": " Visālanettā hasulā sumajjhā,",
  "anya-e0605n10:53_0": "Nimbatthanī vimhayagītasaddā;",
  "anya-e0605n10:54_0": "Alaṅkatā malladharā suvatthā,",
  "anya-e0605n10:55_0": "Naccanti tāḷāvacarehi ghuṭṭhā.",
  "anya-e0605n10:56_0": "76.",
  "anya-e0605n10:57_0": " Yāsaṁ hi loke upamā natthi,",
  "anya-e0605n10:58_0": "Tāsaṁ hi phassesu kathāvakāsā;",
  "anya-e0605n10:59_0": "Taṁ tādisaṁ kāmaratiṁnubhonto,",
  "anya-e0605n10:60_0": "Hitvā kathaṁ nu padamuddhari so nirāso.",
  "anya-e0605n10:61_0": "77.",
  "anya-e0605n10:62_0": " Pādepāde valayaviravāmekhalāvīṇānādā,",
  "anya-e0605n10:63_0": "Gītaṁgītaṁ patiratikaraṁ gāyatī gāyatī sā;",
  "anya-e0605n10:64_0": "Hatthehatthe valayacalitā sambhamaṁ sambhamanti,",
  "anya-e0605n10:65_0": "Disvādisvā iti ratikaraṁ yāti hāhā kimīhā."
}