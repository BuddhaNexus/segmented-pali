{
  "tha-ap235:0.1_0": "Therāpadāna",
  "tha-ap235:0.2_0": "Udakāsanavagga",
  "tha-ap235:0.3_0": "3. Sālapupphiyattheraapadāna",
  "tha-ap235:1.1_0": "“Aruṇavatiyā nagare,",
  "tha-ap235:1.2_0": "ahosiṁ pūpiko tadā;",
  "tha-ap235:1.3_0": "Mama dvārena gacchantaṁ,",
  "tha-ap235:1.4_0": "sikhinaṁ addasaṁ jinaṁ.",
  "tha-ap235:2.1_0": "Buddhassa pattaṁ paggayha,",
  "tha-ap235:2.2_0": "sālapupphaṁ adāsahaṁ;",
  "tha-ap235:2.3_0": "Sammaggatassa buddhassa,",
  "tha-ap235:2.4_0": "vippasannena cetasā.",
  "tha-ap235:3.1_0": "Ekattiṁse ito kappe,",
  "tha-ap235:3.2_0": "Yaṁ pupphamabhidāsahaṁ;",
  "tha-ap235:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap235:3.4_0": "Sālapupphassidaṁ phalaṁ.",
  "tha-ap235:4.1_0": "Ito cuddasakappamhi,",
  "tha-ap235:4.2_0": "ahosiṁ amitañjalo;",
  "tha-ap235:4.3_0": "Sattaratanasampanno,",
  "tha-ap235:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap235:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap235:5.2_0": "…pe…",
  "tha-ap235:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap235:6.1_0": "Itthaṁ sudaṁ āyasmā sālapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap235:7.1_0": "Sālapupphiyattherassāpadānaṁ tatiyaṁ."
}