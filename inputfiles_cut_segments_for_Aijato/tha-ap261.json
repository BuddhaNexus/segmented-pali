{
  "tha-ap261:0.1_0": "Therāpadāna",
  "tha-ap261:0.2_0": "Thomakavagga",
  "tha-ap261:0.3_0": "9. Kuṭidāyakattheraapadāna",
  "tha-ap261:1.1_0": "“Vipinacārī sambuddho,",
  "tha-ap261:1.2_0": "rukkhamūle vasī tadā;",
  "tha-ap261:1.3_0": "Paṇṇasālaṁ karitvāna,",
  "tha-ap261:1.4_0": "adāsiṁ aparājite.",
  "tha-ap261:2.1_0": "Ekanavutito kappe,",
  "tha-ap261:2.2_0": "yaṁ paṇṇakuṭikaṁ adaṁ;",
  "tha-ap261:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap261:2.4_0": "kuṭidānassidaṁ phalaṁ.",
  "tha-ap261:3.1_0": "Aṭṭhavīse ito kappe,",
  "tha-ap261:3.2_0": "soḷasāsiṁsu rājāno;",
  "tha-ap261:3.3_0": "Sabbattha abhivassīti,",
  "tha-ap261:3.4_0": "vuccare cakkavattino.",
  "tha-ap261:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap261:4.2_0": "…pe…",
  "tha-ap261:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap261:5.1_0": "Itthaṁ sudaṁ āyasmā kuṭidāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap261:6.1_0": "Kuṭidāyakattherassāpadānaṁ navamaṁ."
}