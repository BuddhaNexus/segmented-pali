{
  "tha-ap174:0.1_0": "Therāpadāna",
  "tha-ap174:0.2_0": "Kumudavagga",
  "tha-ap174:0.3_0": "2 Nisseṇidāyakattheraapadāna",
  "tha-ap174:1.1_0": "“Koṇḍaññassa bhagavato,",
  "tha-ap174:1.2_0": "lokajeṭṭhassa tādino;",
  "tha-ap174:1.3_0": "Ārohatthāya pāsādaṁ,",
  "tha-ap174:1.4_0": "nisseṇī kāritā mayā.",
  "tha-ap174:2.1_0": "Tena cittappasādena,",
  "tha-ap174:2.2_0": "anubhotvāna sampadā;",
  "tha-ap174:2.3_0": "Dhāremi antimaṁ dehaṁ,",
  "tha-ap174:2.4_0": "sammāsambuddhasāsane.",
  "tha-ap174:3.1_0": "Ekattiṁsamhi kappānaṁ,",
  "tha-ap174:3.2_0": "sahassamhi tayo ahuṁ;",
  "tha-ap174:3.3_0": "Sambahulā nāma rājāno,",
  "tha-ap174:3.4_0": "cakkavattī mahabbalā.",
  "tha-ap174:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap174:4.2_0": "…pe…",
  "tha-ap174:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap174:5.1_0": "Itthaṁ sudaṁ āyasmā nisseṇidāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap174:6.1_0": "Nisseṇidāyakattherassāpadānaṁ dutiyaṁ."
}