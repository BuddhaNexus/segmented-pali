{
  "vv48:0.1_0": "Vimānavatthu",
  "vv48:0.2_0": "Itthivimāna",
  "vv48:0.3_0": "Mañjiṭṭhakavagga",
  "vv48:0.4_0": "10. Ucchuvimānavatthu",
  "vv48:1.1_0": "“Obhāsayitvā pathaviṁ sadevakaṁ,",
  "vv48:1.2_0": "Atirocasi candimasūriyā viya;",
  "vv48:1.3_0": "Siriyā ca vaṇṇena yasena tejasā,",
  "vv48:1.4_0": "Brahmāva deve tidase sahindake.",
  "vv48:2.1_0": "Pucchāmi taṁ uppalamāladhārinī,",
  "vv48:2.2_0": "Āveḷinī kañcanasannibhattace;",
  "vv48:2.3_0": "Alaṅkate uttamavatthadhārinī,",
  "vv48:2.4_0": "Kā tvaṁ subhe devate vandase mamaṁ.",
  "vv48:3.1_0": "Kiṁ tvaṁ pure kammamakāsi attanā,",
  "vv48:3.2_0": "Manussabhūtā purimāya jātiyā;",
  "vv48:3.3_0": "Dānaṁ suciṇṇaṁ atha sīlasaññamaṁ,",
  "vv48:3.4_0": "Kenupapannā sugatiṁ yasassinī;",
  "vv48:3.5_0": "Devate pucchitācikkha,",
  "vv48:3.6_0": "Kissa kammassidaṁ phalan”ti.",
  "vv48:4.1_0": "“Idāni bhante imameva gāmaṁ,",
  "vv48:4.2_0": "Piṇḍāya amhāka gharaṁ upāgami;",
  "vv48:4.3_0": "Tato te ucchussa adāsi khaṇḍikaṁ,",
  "vv48:4.4_0": "Pasannacittā atulāya pītiyā.",
  "vv48:5.1_0": "Sassu ca pacchā anuyuñjate mamaṁ,",
  "vv48:5.2_0": "Kahaṁ nu ucchuṁ vadhuke avākiri;",
  "vv48:5.3_0": "Na chaḍḍitaṁ no pana khāditaṁ mayā,",
  "vv48:5.4_0": "Santassa bhikkhussa sayaṁ adāsahaṁ.",
  "vv48:6.1_0": "Tuyhaṁ nvidaṁ issariyaṁ atho mama,",
  "vv48:6.2_0": "Itissā sassu paribhāsate mamaṁ;",
  "vv48:6.3_0": "Leḍḍuṁ gahetvā pahāraṁ adāsi me,",
  "vv48:6.4_0": "Tato cutā kālakatāmhi devatā.",
  "vv48:7.1_0": "Tadeva kammaṁ kusalaṁ kataṁ mayā,",
  "vv48:7.2_0": "Sukhañca kammaṁ anubhomi attanā;",
  "vv48:7.3_0": "Devehi saddhiṁ paricārayāmahaṁ,",
  "vv48:7.4_0": "Modāmahaṁ kāmaguṇehi pañcahi.",
  "vv48:8.1_0": "Tadeva kammaṁ kusalaṁ kataṁ mayā,",
  "vv48:8.2_0": "Sukhañca kammaṁ anubhomi attanā;",
  "vv48:8.3_0": "Devindaguttā tidasehi rakkhitā,",
  "vv48:8.4_0": "Samappitā kāmaguṇehi pañcahi.",
  "vv48:9.1_0": "Etādisaṁ puññaphalaṁ anappakaṁ,",
  "vv48:9.2_0": "Mahāvipākā mama ucchudakkhiṇā;",
  "vv48:9.3_0": "Devehi saddhiṁ paricārayāmahaṁ,",
  "vv48:9.4_0": "Modāmahaṁ kāmaguṇehi pañcahi.",
  "vv48:10.1_0": "Etādisaṁ puññaphalaṁ anappakaṁ,",
  "vv48:10.2_0": "Mahājutikā mama ucchudakkhiṇā;",
  "vv48:10.3_0": "Devindaguttā tidasehi rakkhitā,",
  "vv48:10.4_0": "Sahassanettoriva nandane vane.",
  "vv48:11.1_0": "Tuvañca bhante anukampakaṁ viduṁ,",
  "vv48:11.2_0": "Upecca vandiṁ kusalañca pucchisaṁ;",
  "vv48:11.3_0": "Tato te ucchussa adāsiṁ khaṇḍikaṁ,",
  "vv48:11.4_0": "Pasannacittā atulāya pītiyā”ti.",
  "vv48:12.1_0": "Ucchuvimānaṁ dasamaṁ."
}