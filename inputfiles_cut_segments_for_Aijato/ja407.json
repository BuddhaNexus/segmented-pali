{
  "ja407:0.1_0": "Jātaka",
  "ja407:0.2_0": "Sattakanipāta",
  "ja407:0.3_0": "Gandhāravagga",
  "ja407:0.4_0": "2. Mahākapijātaka",
  "ja407:1.1_0": "“Attānaṁ saṅkamaṁ katvā,",
  "ja407:1.2_0": "Yo sotthiṁ samatārayi;",
  "ja407:1.3_0": "Kiṁ tvaṁ tesaṁ kime tuyhaṁ,",
  "ja407:1.4_0": "Honti ete mahākapi”.",
  "ja407:2.1_0": "“Rājāhaṁ issaro tesaṁ,",
  "ja407:2.2_0": "yūthassa parihārako;",
  "ja407:2.3_0": "Tesaṁ sokaparetānaṁ,",
  "ja407:2.4_0": "bhītānaṁ te arindama.",
  "ja407:3.1_0": "Ullaṅghayitvā attānaṁ,",
  "ja407:3.2_0": "vissaṭṭhadhanuno sataṁ;",
  "ja407:3.3_0": "Tato aparapādesu,",
  "ja407:3.4_0": "daḷhaṁ bandhaṁ latāguṇaṁ.",
  "ja407:4.1_0": "Chinnabbhamiva vātena,",
  "ja407:4.2_0": "Nuṇṇo rukkhaṁ upāgamiṁ;",
  "ja407:4.3_0": "Sohaṁ appabhavaṁ tattha,",
  "ja407:4.4_0": "Sākhaṁ hatthehi aggahiṁ.",
  "ja407:5.1_0": "Taṁ maṁ viyāyataṁ santaṁ,",
  "ja407:5.2_0": "sākhāya ca latāya ca;",
  "ja407:5.3_0": "Samanukkamantā pādehi,",
  "ja407:5.4_0": "sotthiṁ sākhāmigā gatā.",
  "ja407:6.1_0": "Taṁ maṁ na tapate bandho,",
  "ja407:6.2_0": "mato me na tapessati;",
  "ja407:6.3_0": "Sukhamāharitaṁ tesaṁ,",
  "ja407:6.4_0": "yesaṁ rajjamakārayiṁ.",
  "ja407:7.1_0": "Esā te upamā rāja,",
  "ja407:7.2_0": "taṁ suṇohi arindama;",
  "ja407:7.3_0": "Raññā raṭṭhassa yoggassa,",
  "ja407:7.4_0": "balassa nigamassa ca;",
  "ja407:7.5_0": "Sabbesaṁ sukhameṭṭhabbaṁ,",
  "ja407:7.6_0": "khattiyena pajānatā”ti.",
  "ja407:8.1_0": "Mahākapijātakaṁ dutiyaṁ."
}