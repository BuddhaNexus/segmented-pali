{
  "kv23.2:0.1_0": "Kathāvatthu",
  "kv23.2:0.2_0": "Mahāpaṇṇāsaka",
  "kv23.2:0.3_0": "Tevīsatimavagga",
  "kv23.2:0.4_0": "Arahantavaṇṇakathā",
  "kv23.2:1.1_0": "Arahantānaṁ vaṇṇena amanussā methunaṁ dhammaṁ paṭisevantīti?",
  "kv23.2:1.2_0": "Āmantā.",
  "kv23.2:1.3_0": "Arahantānaṁ vaṇṇena amanussā pāṇaṁ hananti …pe…",
  "kv23.2:1.4_0": "adinnaṁ ādiyanti,",
  "kv23.2:1.5_0": "musā bhaṇanti,",
  "kv23.2:1.6_0": "pisuṇaṁ bhaṇanti,",
  "kv23.2:1.7_0": "pharusaṁ bhaṇanti,",
  "kv23.2:1.8_0": "samphaṁ palapanti,",
  "kv23.2:1.9_0": "sandhiṁ chindanti,",
  "kv23.2:1.10_0": "nillopaṁ haranti,",
  "kv23.2:1.11_0": "ekāgārikaṁ karonti,",
  "kv23.2:1.12_0": "paripanthe tiṭṭhanti,",
  "kv23.2:1.13_0": "paradāraṁ gacchanti,",
  "kv23.2:1.14_0": "gāmaghātakaṁ karonti …pe…",
  "kv23.2:1.15_0": "nigamaghātakaṁ karontīti?",
  "kv23.2:1.16_0": "Na hevaṁ vattabbe …pe….",
  "kv23.2:1.17_0": "Arahantavaṇṇakathā niṭṭhitā."
}