{
  "tha-ap88:0.1_0": "Therāpadāna",
  "tha-ap88:0.2_0": "Timiravagga",
  "tha-ap88:0.3_0": "6 Dutiyaraṁsisaññakattheraapadāna",
  "tha-ap88:1.1_0": "“Pabbate himavantamhi,",
  "tha-ap88:1.2_0": "vākacīradharo ahaṁ;",
  "tha-ap88:1.3_0": "Caṅkamañca samārūḷho,",
  "tha-ap88:1.4_0": "nisīdiṁ pācināmukho.",
  "tha-ap88:2.1_0": "Pabbate sugataṁ disvā,",
  "tha-ap88:2.2_0": "phussaṁ jhānarataṁ tadā;",
  "tha-ap88:2.3_0": "Añjaliṁ paggahetvāna,",
  "tha-ap88:2.4_0": "raṁsyā cittaṁ pasādayiṁ.",
  "tha-ap88:3.1_0": "Dvenavute ito kappe,",
  "tha-ap88:3.2_0": "yaṁ saññamalabhiṁ tadā;",
  "tha-ap88:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap88:3.4_0": "raṁsisaññāyidaṁ phalaṁ.",
  "tha-ap88:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap88:4.2_0": "…pe…",
  "tha-ap88:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap88:5.1_0": "Itthaṁ sudaṁ āyasmā raṁsisaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap88:6.1_0": "Dutiyaraṁsisaññakattherassāpadānaṁ chaṭṭhaṁ."
}