{
  "tha-ap460:0.1_0": "Therāpadāna",
  "tha-ap460:0.2_0": "Jagatidāyakavagga",
  "tha-ap460:0.3_0": "8. Jātipupphiyattheraapadāna",
  "tha-ap460:1.1_0": "“Parinibbute bhagavati,",
  "tha-ap460:1.2_0": "padumuttare mahāyase;",
  "tha-ap460:1.3_0": "Pupphavaṭaṁsake katvā,",
  "tha-ap460:1.4_0": "sarīramabhiropayiṁ.",
  "tha-ap460:2.1_0": "Tattha cittaṁ pasādetvā,",
  "tha-ap460:2.2_0": "nimmānaṁ agamāsahaṁ;",
  "tha-ap460:2.3_0": "Devalokagato santo,",
  "tha-ap460:2.4_0": "puññakammaṁ sarāmahaṁ.",
  "tha-ap460:3.1_0": "Ambarā pupphavasso me,",
  "tha-ap460:3.2_0": "sabbakālaṁ pavassati;",
  "tha-ap460:3.3_0": "Saṁsarāmi manusse ce,",
  "tha-ap460:3.4_0": "rājā homi mahāyaso.",
  "tha-ap460:4.1_0": "Tahiṁ kusumavasso me,",
  "tha-ap460:4.2_0": "abhivassati sabbadā;",
  "tha-ap460:4.3_0": "Tasseva pupphapūjāya,",
  "tha-ap460:4.4_0": "vāhasā sabbadassino.",
  "tha-ap460:5.1_0": "Ayaṁ pacchimako mayhaṁ,",
  "tha-ap460:5.2_0": "carimo vattate bhavo;",
  "tha-ap460:5.3_0": "Ajjāpi pupphavasso me,",
  "tha-ap460:5.4_0": "abhivassati sabbadā.",
  "tha-ap460:6.1_0": "Satasahassito kappe,",
  "tha-ap460:6.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap460:6.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap460:6.4_0": "dehapūjāyidaṁ phalaṁ.",
  "tha-ap460:7.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap460:7.2_0": "…pe…",
  "tha-ap460:7.3_0": "viharāmi anāsavo.",
  "tha-ap460:8.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap460:8.2_0": "…pe…",
  "tha-ap460:8.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap460:9.1_0": "Paṭisambhidā catasso,",
  "tha-ap460:9.2_0": "…pe…",
  "tha-ap460:9.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap460:10.1_0": "Itthaṁ sudaṁ āyasmā jātipupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap460:11.1_0": "Jātipupphiyattherassāpadānaṁ aṭṭhamaṁ."
}