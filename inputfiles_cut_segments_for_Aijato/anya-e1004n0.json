{
  "anya-e1004n0:0_0": " Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1004n0:1_0": "Mahārahanīti",
  "anya-e1004n0:2_0": "Paṇḍitakathā",
  "anya-e1004n0:3_0": "1.",
  "anya-e1004n0:4_0": " Mahāraharahaṁ sakya,",
  "anya-e1004n0:5_0": "Muniṁ nīvaraṇā taṇhā;",
  "anya-e1004n0:6_0": "Muttaṁ muttaṁ sudassanaṁ,",
  "anya-e1004n0:7_0": "Vande bodhivaraṁ varaṁ.",
  "anya-e1004n0:8_0": "2.",
  "anya-e1004n0:9_0": " Muninā munināgena,",
  "anya-e1004n0:10_0": "Duṭṭhapabbajjitā jitā;",
  "anya-e1004n0:11_0": "Yathā yathāasaddhamma-",
  "anya-e1004n0:12_0": "Puraṇā puraṇādayo.",
  "anya-e1004n0:13_0": "3.",
  "anya-e1004n0:14_0": " Tathā tathāgatovādā,",
  "anya-e1004n0:15_0": "Nugate lokanītiyaṁ;",
  "anya-e1004n0:16_0": "Katayogena vidūnā,",
  "anya-e1004n0:17_0": "Jeyyovānattha kārino.",
  "anya-e1004n0:18_0": "4.",
  "anya-e1004n0:19_0": " Nitīdha jantunaṁ sāro,",
  "anya-e1004n0:20_0": "Mittācariyāca pītarā;",
  "anya-e1004n0:21_0": "Nītimā subuddhi byatto,",
  "anya-e1004n0:22_0": "Sutavā atthadassimā.",
  "anya-e1004n0:23_0": "5.",
  "anya-e1004n0:24_0": " Kaviheraññakā katvā,",
  "anya-e1004n0:25_0": "Suttattaṁ satthakañcanaṁ;",
  "anya-e1004n0:26_0": "Gajjapajjādyālaṅkāraṁ,",
  "anya-e1004n0:27_0": "Karontīdha manorammaṁ.",
  "anya-e1004n0:28_0": "6.",
  "anya-e1004n0:29_0": " Vutyaṁ visadañāṇassa,",
  "anya-e1004n0:30_0": "Ñāto atthotarassa na;",
  "anya-e1004n0:31_0": "Surappabhāya ādāso,",
  "anya-e1004n0:32_0": "Chāyaṁ disse na mākare.",
  "anya-e1004n0:33_0": "7.",
  "anya-e1004n0:34_0": " Mahātejopi tejoyaṁ,",
  "anya-e1004n0:35_0": "Mattikaṁ namudukaṁkaro;",
  "anya-e1004n0:36_0": "Āpo āpeti mudukaṁ,",
  "anya-e1004n0:37_0": "Sādhuvācāca kakkhaḷaṁ.",
  "anya-e1004n0:38_0": "8.",
  "anya-e1004n0:39_0": " Mudunāva ripuṁ jeti,",
  "anya-e1004n0:40_0": "Mudunā jeti dāruṇaṁ;",
  "anya-e1004n0:41_0": "Nosiddhaṁ mudunā kiñci,",
  "anya-e1004n0:42_0": "Yatvato mudunā jaye.",
  "anya-e1004n0:43_0": "9.",
  "anya-e1004n0:44_0": " Candanaṁ sītalaṁ loke,",
  "anya-e1004n0:45_0": "Tato candaṁva sītalaṁ;",
  "anya-e1004n0:46_0": "Candanacandasītamhā,",
  "anya-e1004n0:47_0": "Vākyaṁ sādhusubhāsitaṁ.",
  "anya-e1004n0:48_0": "10.",
  "anya-e1004n0:49_0": " Pattakalloditaṁ appaṁ,",
  "anya-e1004n0:50_0": "Vākyaṁ subhāsitaṁ bhave;",
  "anya-e1004n0:51_0": "Khudditassa kadannaṁpi,",
  "anya-e1004n0:52_0": "Suttaṁ sāduraso siyā.",
  "anya-e1004n0:53_0": "11.",
  "anya-e1004n0:54_0": " Satthakāpi bahūvācā,",
  "anya-e1004n0:55_0": "Nādarā bahubhāṇino;",
  "anya-e1004n0:56_0": "Sopakāramudāsīnaṁ,",
  "anya-e1004n0:57_0": "Nanudiṭṭhaṁ nadījalaṁ.",
  "anya-e1004n0:58_0": "12.",
  "anya-e1004n0:59_0": " Saṁsāravisarukkhassa ,",
  "anya-e1004n0:60_0": "Dvayamevāmataṁphalaṁ;",
  "anya-e1004n0:61_0": "Subhāsitassasārādo,",
  "anya-e1004n0:62_0": "Sādhūhirasamāgamo.",
  "anya-e1004n0:63_0": "13.",
  "anya-e1004n0:64_0": " Pāsāṇachattaṁ garukaṁ,",
  "anya-e1004n0:65_0": "Tato devānacikkhanā;",
  "anya-e1004n0:66_0": "Tato vuḍḍhānamovādo,",
  "anya-e1004n0:67_0": "Tato buddhassa sāsanaṁ;",
  "anya-e1004n0:68_0": "14.",
  "anya-e1004n0:69_0": " Tulaṁ sallahukaṁ loke,",
  "anya-e1004n0:70_0": "Tato capalajātiko;",
  "anya-e1004n0:71_0": "Tatonovādiko tato,",
  "anya-e1004n0:72_0": "Yati dhammepamādako.",
  "anya-e1004n0:73_0": "15.",
  "anya-e1004n0:74_0": " Aha gacchati hāyantī,",
  "anya-e1004n0:75_0": "Sattānamidha jīvitaṁ;",
  "anya-e1004n0:76_0": "Tasmāhi māpamattatvaṁ,",
  "anya-e1004n0:77_0": "Gacchantu jinasāsane.",
  "anya-e1004n0:78_0": "16.",
  "anya-e1004n0:79_0": " Paṇḍitassa pasaṁsāya,",
  "anya-e1004n0:80_0": "Daṇḍobālena dīyate;",
  "anya-e1004n0:81_0": "Paṇḍito paṇḍiteneva,",
  "anya-e1004n0:82_0": "Vaṇṇitova suvaṇṇito.",
  "anya-e1004n0:83_0": "17.",
  "anya-e1004n0:84_0": " Satesu jāyate sūro,",
  "anya-e1004n0:85_0": "Sahassesuca paṇḍito;",
  "anya-e1004n0:86_0": "Vuttā satasahassesu,",
  "anya-e1004n0:87_0": "Dātā bhavativā navā.",
  "anya-e1004n0:88_0": "18.",
  "anya-e1004n0:89_0": " Bhūpattaṁca paṇḍitattañca,",
  "anya-e1004n0:90_0": "Nevatulyaṁ kadācipi;",
  "anya-e1004n0:91_0": "Sadesepūjito rājā,",
  "anya-e1004n0:92_0": "Buddho sabbatthapūjito.",
  "anya-e1004n0:93_0": "19.",
  "anya-e1004n0:94_0": " Kvātibhāro samatthānaṁ,",
  "anya-e1004n0:95_0": "Kiṁdūro byavahārinaṁ;",
  "anya-e1004n0:96_0": "Kovideso savijjānaṁ,",
  "anya-e1004n0:97_0": "Koparo piyavādinaṁ.",
  "anya-e1004n0:98_0": "20.",
  "anya-e1004n0:99_0": " Nanu te yevasantā no,",
  "anya-e1004n0:100_0": "Sāgarā nakulācalā;",
  "anya-e1004n0:101_0": "Appampi mariyādaṁ ye,",
  "anya-e1004n0:102_0": "Saṁvaṭṭevi jahantino.",
  "anya-e1004n0:103_0": "21.",
  "anya-e1004n0:104_0": " Palayetītamariyādā ,",
  "anya-e1004n0:105_0": "Bhavantikira sāgarā;",
  "anya-e1004n0:106_0": "Mariyādātikaṁ sādhu,",
  "anya-e1004n0:107_0": "Naicchanti palayepi.",
  "anya-e1004n0:108_0": "22.",
  "anya-e1004n0:109_0": " Sataṁ dīghāyukaṁ sabba,",
  "anya-e1004n0:110_0": "Sattānaṁ sukhakāraṇaṁ;",
  "anya-e1004n0:111_0": "Asataṁpana sabbesaṁ,",
  "anya-e1004n0:112_0": "Dukkhahetu nasaṁsayo.",
  "anya-e1004n0:113_0": "23.",
  "anya-e1004n0:114_0": " Pāmokkhe sujane sante,",
  "anya-e1004n0:115_0": "Sabbepi sujanā janā;",
  "anya-e1004n0:116_0": "Jātekasmiṁsāragandhe,",
  "anya-e1004n0:117_0": "Sabbe gandhamayā dumā.",
  "anya-e1004n0:118_0": "24.",
  "anya-e1004n0:119_0": " Attanā yadi ekena,",
  "anya-e1004n0:120_0": "Vinatena mahājanā;",
  "anya-e1004n0:121_0": "Vinayaṁ yanti sabbepi,",
  "anya-e1004n0:122_0": "Ko taṁ nāseyya paṇḍito.",
  "anya-e1004n0:123_0": "25.",
  "anya-e1004n0:124_0": " Sarīrassa guṇānañca,",
  "anya-e1004n0:125_0": "Dūramaccanta mantaraṁ;",
  "anya-e1004n0:126_0": "Sarīraṁ khaṇaviddhaṁsi,",
  "anya-e1004n0:127_0": "Kappantiṭṭhāyino guṇā.",
  "anya-e1004n0:128_0": "26.",
  "anya-e1004n0:129_0": " Yadi niccamaniccena,",
  "anya-e1004n0:130_0": "Nimalaṁ malavāhinā;",
  "anya-e1004n0:131_0": "Yo so kāyena labhetha,",
  "anya-e1004n0:132_0": "Kiṁnaladdhaṁ bhave nukiṁ.",
  "anya-e1004n0:133_0": "27.",
  "anya-e1004n0:134_0": " Dhammatthakāmamokkhānaṁ,",
  "anya-e1004n0:135_0": "Pāṇaṁ saṁsiddhikāraṇaṁ;",
  "anya-e1004n0:136_0": "Tannighātā kinnahataṁ,",
  "anya-e1004n0:137_0": "Rakkhanā kinnarakkhitaṁ.",
  "anya-e1004n0:138_0": "28.",
  "anya-e1004n0:139_0": " Attā bandhu manussassa,",
  "anya-e1004n0:140_0": "Ripu attāva jantunaṁ;",
  "anya-e1004n0:141_0": "Attāva niyato ñāti,",
  "anya-e1004n0:142_0": "Attāva niyato ripu.",
  "anya-e1004n0:143_0": "29.",
  "anya-e1004n0:144_0": " Attanoparicāgena,",
  "anya-e1004n0:145_0": "Yaṁsitamanurakkhanaṁ;",
  "anya-e1004n0:146_0": "Karonti sajjanāyeva,",
  "anya-e1004n0:147_0": "Na taṁ nītimatā mataṁ.",
  "anya-e1004n0:148_0": "30.",
  "anya-e1004n0:149_0": " Tiṇāni bhūmirudakaṁ,",
  "anya-e1004n0:150_0": "Catutthī vākyasuṭṭhutā;",
  "anya-e1004n0:151_0": "Etāni hi sataṁgehe,",
  "anya-e1004n0:152_0": "Nochindante kadācipi.",
  "anya-e1004n0:153_0": "31.",
  "anya-e1004n0:154_0": " Ambuṁ pivantino najjo,",
  "anya-e1004n0:155_0": "Rukkho khādatino phalaṁ;",
  "anya-e1004n0:156_0": "Megho kvacipi no sassaṁ,",
  "anya-e1004n0:157_0": "Paratthāya sataṁdhanaṁ.",
  "anya-e1004n0:158_0": "32.",
  "anya-e1004n0:159_0": " Sataṁ pharusavācāhi,",
  "anya-e1004n0:160_0": "Nayāti vikatiṁ mano;",
  "anya-e1004n0:161_0": "Tiṇukkāhi nasakkāva,",
  "anya-e1004n0:162_0": "Tāpetuṁ sāgare jalaṁ.",
  "anya-e1004n0:163_0": "33.",
  "anya-e1004n0:164_0": " Sampatyaṁ mahattaṁceto,",
  "anya-e1004n0:165_0": "Bhavatyuppalakomalaṁ;",
  "anya-e1004n0:166_0": "Vippatyañca mahāsele,",
  "anya-e1004n0:167_0": "Silāsaṅghādakakkaso.",
  "anya-e1004n0:168_0": "34.",
  "anya-e1004n0:169_0": " Atthaṁ mahantamāpajja,",
  "anya-e1004n0:170_0": "Vijjaṁ sampattimevaca;",
  "anya-e1004n0:171_0": "Careyyāmānathaddhoyo,",
  "anya-e1004n0:172_0": "Paṇḍito so pavuccati.",
  "anya-e1004n0:173_0": "35.",
  "anya-e1004n0:174_0": " Nālabbhamabhipatthenti ,",
  "anya-e1004n0:175_0": "Naṭṭhampi nacasocare;",
  "anya-e1004n0:176_0": "Vippatyañca namuyhanti,",
  "anya-e1004n0:177_0": "Yenarā teva paṇḍito.",
  "anya-e1004n0:178_0": "36.",
  "anya-e1004n0:179_0": " Gaṇṭhiṭṭhāne ekapade,",
  "anya-e1004n0:180_0": "Nātimaññeyya paṇḍito;",
  "anya-e1004n0:181_0": "Kiṁ akko veḷupabbhāro,",
  "anya-e1004n0:182_0": "Timahādīpabhānudo.",
  "anya-e1004n0:183_0": "37.",
  "anya-e1004n0:184_0": " Guṇadosesumekena,",
  "anya-e1004n0:185_0": "Natthi koci vivajjito;",
  "anya-e1004n0:186_0": "Sukhumālassa padumassa,",
  "anya-e1004n0:187_0": "Naḷā bhavati kakkasā.",
  "anya-e1004n0:188_0": "38.",
  "anya-e1004n0:189_0": " Sumahantāni satthāni,",
  "anya-e1004n0:190_0": "Dhārayanto bahussutā;",
  "anya-e1004n0:191_0": "Chettāro saṁsayānantu,",
  "anya-e1004n0:192_0": "Kilante lobhamohatā.",
  "anya-e1004n0:193_0": "39.",
  "anya-e1004n0:194_0": " Dosampi saguṇe disvā,",
  "anya-e1004n0:195_0": "Guṇavādī vadantino;",
  "anya-e1004n0:196_0": "Na loko vijjamānampi,",
  "anya-e1004n0:197_0": "Cande passati lañchanaṁ.",
  "anya-e1004n0:198_0": "40.",
  "anya-e1004n0:199_0": " Nipuṇe sutameseyya,",
  "anya-e1004n0:200_0": "Vicinitvā sutatthiko;",
  "anya-e1004n0:201_0": "Bhattaṁ hukkhaliyaṁ pakkaṁ,",
  "anya-e1004n0:202_0": "Bhājanaṁpi tathā bhave.",
  "anya-e1004n0:203_0": "41.",
  "anya-e1004n0:204_0": " Guṇā kubbanti dūtattaṁ,",
  "anya-e1004n0:205_0": "Dūrepi vasataṁ sataṁ;",
  "anya-e1004n0:206_0": "Ketake gandha ghāsāya,",
  "anya-e1004n0:207_0": "Gacchanti bhamarā sayaṁ.",
  "anya-e1004n0:208_0": "42.",
  "anya-e1004n0:209_0": " Paṇḍito sutasampanno,",
  "anya-e1004n0:210_0": "Yatthaatthīti cesuto;",
  "anya-e1004n0:211_0": "Mahussāhena taṁṭhānaṁ,",
  "anya-e1004n0:212_0": "Gantabbaṁ sutaresinā.",
  "anya-e1004n0:213_0": "43.",
  "anya-e1004n0:214_0": " Sabbasutaṁ madhīyetha,",
  "anya-e1004n0:215_0": "Hīnamukkaṭṭha majjhimaṁ;",
  "anya-e1004n0:216_0": "Sabbassaatthaṁ jāneyya,",
  "anya-e1004n0:217_0": "Naca sabbaṁ payojaye;",
  "anya-e1004n0:218_0": "Hoti tādisakokālo,",
  "anya-e1004n0:219_0": "Yattha atthāvahaṁ sutaṁ.",
  "anya-e1004n0:220_0": "44.",
  "anya-e1004n0:221_0": " Ṭhito cirattanaṁ nīre,",
  "anya-e1004n0:222_0": "Sakkharo nantaraṁsuto;",
  "anya-e1004n0:223_0": "Dabbho nīrantike jāto,",
  "anya-e1004n0:224_0": "Sabbakālantaraṁsuto.",
  "anya-e1004n0:225_0": "45.",
  "anya-e1004n0:226_0": " Vasuṁ gaṇhanti dūraṭṭhā,",
  "anya-e1004n0:227_0": "Pabbate ratanocite;",
  "anya-e1004n0:228_0": "Na milakkhā samīpaṭṭhā,",
  "anya-e1004n0:229_0": "Evaṁ bālā bahussute.",
  "anya-e1004n0:230_0": "46.",
  "anya-e1004n0:231_0": " Hiraññena migānaṁva,",
  "anya-e1004n0:232_0": "Susīlaṁpi asīlino;",
  "anya-e1004n0:233_0": "Adhammikassa dhammena,",
  "anya-e1004n0:234_0": "Bālānaṁpi sutena kiṁ.",
  "anya-e1004n0:235_0": "47.",
  "anya-e1004n0:236_0": " Sutena hadayaṭṭhena,",
  "anya-e1004n0:237_0": "Khalo nevasusīlavā;",
  "anya-e1004n0:238_0": "Madhunā koṭaraṭṭhena,",
  "anya-e1004n0:239_0": "Nimbo kiṁ madhurāyate.",
  "anya-e1004n0:240_0": "48.",
  "anya-e1004n0:241_0": " Sakiṁpi viññu dhīrena,",
  "anya-e1004n0:242_0": "Karoti sahasaṅgamaṁ;",
  "anya-e1004n0:243_0": "Attatthañca paratthañca,",
  "anya-e1004n0:244_0": "Nibbānantaṁ sukhaṁ labhe.",
  "anya-e1004n0:245_0": "49.",
  "anya-e1004n0:246_0": " Haṁso majjhe nakākānaṁ,",
  "anya-e1004n0:247_0": "Sīho gunnaṁ nasobhati;",
  "anya-e1004n0:248_0": "Gadrabhānaṁ turaṅgono,",
  "anya-e1004n0:249_0": "Bālānañca napaṇḍito.",
  "anya-e1004n0:250_0": "50.",
  "anya-e1004n0:251_0": " Ākiṇṇopi asabbhīva,",
  "anya-e1004n0:252_0": "Asaṁsaṭṭhova bhaddako;",
  "anya-e1004n0:253_0": "Bahunā sannajātena,",
  "anya-e1004n0:254_0": "Gacche nummattakenidha.",
  "anya-e1004n0:255_0": "51.",
  "anya-e1004n0:256_0": " Nadītire khatekūpe,",
  "anya-e1004n0:257_0": "Araṇītālavaṇṭake;",
  "anya-e1004n0:258_0": "Navade āpādi natthīti,",
  "anya-e1004n0:259_0": "Mukheca vacanaṁ tathā.",
  "anya-e1004n0:260_0": "52.",
  "anya-e1004n0:261_0": " Sutasanniccayā dhīrā,",
  "anya-e1004n0:262_0": "Tuṇhibhūtā apucchitā;",
  "anya-e1004n0:263_0": "Puṇṇā subhāsitenāpi,",
  "anya-e1004n0:264_0": "Ghaṇḍādyaghaṭitāyathā.",
  "anya-e1004n0:265_0": "53.",
  "anya-e1004n0:266_0": " Apuṭṭho paṇḍito bherī,",
  "anya-e1004n0:267_0": "Pajjunno hoti pucchito;",
  "anya-e1004n0:268_0": "Bālo puṭṭho apuṭṭhoca,",
  "anya-e1004n0:269_0": "Bahuṁ vikattate sadā.",
  "anya-e1004n0:270_0": "54.",
  "anya-e1004n0:271_0": " Sampannaguṇā laṅkāro,",
  "anya-e1004n0:272_0": "Sabbasatta hitāvaho;",
  "anya-e1004n0:273_0": "Nacareyya parattatthaṁ,",
  "anya-e1004n0:274_0": "Kuto so paṇḍito bhave.",
  "anya-e1004n0:275_0": "55.",
  "anya-e1004n0:276_0": " Saparatthaṁ care dhīro,",
  "anya-e1004n0:277_0": "Asakkonto sakaṁ care;",
  "anya-e1004n0:278_0": "Tathāpica asakkonto,",
  "anya-e1004n0:279_0": "Pāpāttānaṁ viyojaye.",
  "anya-e1004n0:280_0": "56.",
  "anya-e1004n0:281_0": " Bālecummattake bhūpe,",
  "anya-e1004n0:282_0": "Gurumātāpitūsvapi;",
  "anya-e1004n0:283_0": "Nadosaṁ kariyā pañño,",
  "anya-e1004n0:284_0": "Saṅgheca jeṭṭhabhātari.",
  "anya-e1004n0:285_0": "57.",
  "anya-e1004n0:286_0": " Atthanāsaṁ manotāpaṁ,",
  "anya-e1004n0:287_0": "Ghareduccaritānica;",
  "anya-e1004n0:288_0": "Vañcanaṁ avamānañca,",
  "anya-e1004n0:289_0": "Matimā napakāsaye.",
  "anya-e1004n0:290_0": "58.",
  "anya-e1004n0:291_0": " Paradānaṁ janettīva,",
  "anya-e1004n0:292_0": "Leṭṭuva paradabbakaṁ;",
  "anya-e1004n0:293_0": "Attāva sabbabhūtāni,",
  "anya-e1004n0:294_0": "Yo passati so passati.",
  "anya-e1004n0:295_0": "59.",
  "anya-e1004n0:296_0": " Parūpavāde padhiro,",
  "anya-e1004n0:297_0": "Paravajjesvalocano;",
  "anya-e1004n0:298_0": "Paṅgulo aññanārīsu,",
  "anya-e1004n0:299_0": "Dussakappesvacetano.",
  "anya-e1004n0:300_0": "60.",
  "anya-e1004n0:301_0": " Sabbaṁ suṇāti sotena,",
  "anya-e1004n0:302_0": "Sabbaṁ passati cakkhunā;",
  "anya-e1004n0:303_0": "Nacadiṭṭhaṁ sutaṁ dhīro,",
  "anya-e1004n0:304_0": "Sabbaṁ uccitumarahati.",
  "anya-e1004n0:305_0": "61.",
  "anya-e1004n0:306_0": " Cakkhumassa yathāandho,",
  "anya-e1004n0:307_0": "Sotavā padhiroyathā;",
  "anya-e1004n0:308_0": "Paññāvāssa yathāmūgo,",
  "anya-e1004n0:309_0": "Balavā dubbaloriva;",
  "anya-e1004n0:310_0": "Atha atthesamuppanne,",
  "anya-e1004n0:311_0": "Sayetha matasāyitaṁ.",
  "anya-e1004n0:312_0": "62.",
  "anya-e1004n0:313_0": " Pāpamitte vivajjetvā,",
  "anya-e1004n0:314_0": "Bhajeyyu ttamapuggalaṁ;",
  "anya-e1004n0:315_0": "Ovādecassa tiṭṭheyya,",
  "anya-e1004n0:316_0": "Patthento acalaṁsukhaṁ.",
  "anya-e1004n0:317_0": "63.",
  "anya-e1004n0:318_0": " Sususā sutavaḍḍhanaṁ,",
  "anya-e1004n0:319_0": "Sutaṁ paññāyavaḍḍhanaṁ;",
  "anya-e1004n0:320_0": "Paññāya atthaṁ jānāti,",
  "anya-e1004n0:321_0": "Ñāto attho sukhāvaho.",
  "anya-e1004n0:322_0": "64.",
  "anya-e1004n0:323_0": " Pāmojja karaṇaṁ ṭhānaṁ,",
  "anya-e1004n0:324_0": "Pasaṁsāvahanaṁsukhaṁ;",
  "anya-e1004n0:325_0": "Phalānisaṁso bhāveti,",
  "anya-e1004n0:326_0": "Vahanto porissaṁdhuraṁ.",
  "anya-e1004n0:327_0": "65.",
  "anya-e1004n0:328_0": " Atisītaṁ atiuṇhaṁ,",
  "anya-e1004n0:329_0": "Atisāya midaṁahu;",
  "anya-e1004n0:330_0": "Iti visaṭṭha kammante,",
  "anya-e1004n0:331_0": "Khaṇā accanti māṇave.",
  "anya-e1004n0:332_0": "66.",
  "anya-e1004n0:333_0": " Yoca sītañca uṇhañca,",
  "anya-e1004n0:334_0": "Tiṇātiyo namaññati;",
  "anya-e1004n0:335_0": "Karaṁ purisa kiccāni,",
  "anya-e1004n0:336_0": "So sukhā navihāyati.",
  "anya-e1004n0:337_0": "67.",
  "anya-e1004n0:338_0": " Yasalābhaṁ jigīsantaṁ,",
  "anya-e1004n0:339_0": "Naraṁ vajjenti dūrato;",
  "anya-e1004n0:340_0": "Apatthetvāna te tasmā,",
  "anya-e1004n0:341_0": "Taṁmaggaṁ maggaye budho.",
  "anya-e1004n0:342_0": "68.",
  "anya-e1004n0:343_0": " Desamosajja gacchanti,",
  "anya-e1004n0:344_0": "Sīhā sappurisā gajā;",
  "anya-e1004n0:345_0": "Tattheva nidhanaṁ yanti,",
  "anya-e1004n0:346_0": "Kākā kāpurisā migā.",
  "anya-e1004n0:347_0": "69.",
  "anya-e1004n0:348_0": " Yasmiṁdese nasammāro,",
  "anya-e1004n0:349_0": "Napiti nacabandhavā;",
  "anya-e1004n0:350_0": "Nacavijjāgamo koci,",
  "anya-e1004n0:351_0": "Natattha vasatī vasse.",
  "anya-e1004n0:352_0": "70.",
  "anya-e1004n0:353_0": " Dhanavā gaṇako rājā,",
  "anya-e1004n0:354_0": "Nadī vejjā imejanā;",
  "anya-e1004n0:355_0": "Yattha pañca navijjante,",
  "anya-e1004n0:356_0": "Na tatthavasatī vase.",
  "anya-e1004n0:357_0": "71.",
  "anya-e1004n0:358_0": " Asanaṁ bhayamantānaṁ,",
  "anya-e1004n0:359_0": "Maccānaṁ maraṇaṁ bhayaṁ;",
  "anya-e1004n0:360_0": "Uttamānantusabbesaṁ,",
  "anya-e1004n0:361_0": "Avamānaṁ paraṁ bhayaṁ.",
  "anya-e1004n0:362_0": "72.",
  "anya-e1004n0:363_0": " Amānanā yattha siyā,",
  "anya-e1004n0:364_0": "Santānaṁ avamānanā;",
  "anya-e1004n0:365_0": "Hīnasamānanāvāpi,",
  "anya-e1004n0:366_0": "Na tatthavasati vase.",
  "anya-e1004n0:367_0": "73.",
  "anya-e1004n0:368_0": " Yatthālasoca dakkhoca,",
  "anya-e1004n0:369_0": "Sūro bhīruca pūjito;",
  "anya-e1004n0:370_0": "Na santā tattha vassanti,",
  "anya-e1004n0:371_0": "Avisesakarena ko.",
  "anya-e1004n0:372_0": "74.",
  "anya-e1004n0:373_0": " Dukkho nivāso sambādhe,",
  "anya-e1004n0:374_0": "Ṭhāne asucisaṅkaṭe;",
  "anya-e1004n0:375_0": "Tato rimhipiyānante,",
  "anya-e1004n0:376_0": "Tatoca akataññunaṁ.",
  "anya-e1004n0:377_0": "75.",
  "anya-e1004n0:378_0": " Siṅgīnaṁ pañcahatthena,",
  "anya-e1004n0:379_0": "Satena vājinaṁ caje;",
  "anya-e1004n0:380_0": "Hatthīnantu sahassena,",
  "anya-e1004n0:381_0": "Desacāgena dujjane.",
  "anya-e1004n0:382_0": "76.",
  "anya-e1004n0:383_0": " Caje ekaṁ kulassatthaṁ,",
  "anya-e1004n0:384_0": "Gāmassatthaṁ kulaṁ caje;",
  "anya-e1004n0:385_0": "Gāmaṁ janapadassatthaṁ;",
  "anya-e1004n0:386_0": "Attatthaṁ pathavī caje.",
  "anya-e1004n0:387_0": "77.",
  "anya-e1004n0:388_0": " Calatyekenapādena,",
  "anya-e1004n0:389_0": "Tiṭṭhatyekena paṇḍito;",
  "anya-e1004n0:390_0": "Asamikhyaparaṁṭhānaṁ,",
  "anya-e1004n0:391_0": "Pubbamāyanaṁnacaje.",
  "anya-e1004n0:392_0": "78.",
  "anya-e1004n0:393_0": " Ṭhānabhaṭṭhā nasobhanti,",
  "anya-e1004n0:394_0": "Dantākesāna khānarā;",
  "anya-e1004n0:395_0": "Matimā itiviññāya,",
  "anya-e1004n0:396_0": "Saṭhānaṁ nacaje lahuṁ.",
  "anya-e1004n0:397_0": "79.",
  "anya-e1004n0:398_0": " Caje dhanaṁ aṅgavarassahetu,",
  "anya-e1004n0:399_0": "Aṅgaṁ caje jivitaṁ rakkhamāno;",
  "anya-e1004n0:400_0": "Aṅgaṁ dhanaṁ jivitañcāpi sabbaṁ,",
  "anya-e1004n0:401_0": "Caje naro dhamma manussaranto.",
  "anya-e1004n0:402_0": "80.",
  "anya-e1004n0:403_0": " Sotaṁ suteneva na kuṇḍalena,",
  "anya-e1004n0:404_0": "Dānena pāṇī natu kaṅkaṇena;",
  "anya-e1004n0:405_0": "Ābhāti kāyo purisuttamassa,",
  "anya-e1004n0:406_0": "Paropakārena na candanena."
}