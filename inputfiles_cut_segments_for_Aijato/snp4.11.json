{
  "snp4.11:0.1_0": "Sutta Nipāta 4.11",
  "snp4.11:0.2_0": "Kalahavivādasutta",
  "snp4.11:1.1_0": "“Kutopahūtā kalahā vivādā,",
  "snp4.11:1.2_0": "Paridevasokā sahamaccharā ca;",
  "snp4.11:1.3_0": "Mānātimānā sahapesuṇā ca,",
  "snp4.11:1.4_0": "Kutopahūtā te tadiṅgha brūhi”.",
  "snp4.11:2.1_0": "“Piyappahūtā kalahā vivādā,",
  "snp4.11:2.2_0": "Paridevasokā sahamaccharā ca;",
  "snp4.11:2.3_0": "Mānātimānā sahapesuṇā ca,",
  "snp4.11:2.4_0": "Maccherayuttā kalahā vivādā;",
  "snp4.11:2.5_0": "Vivādajātesu ca pesuṇāni”.",
  "snp4.11:3.1_0": "“Piyā su lokasmiṁ kutonidānā,",
  "snp4.11:3.2_0": "Ye cāpi lobhā vicaranti loke;",
  "snp4.11:3.3_0": "Āsā ca niṭṭhā ca kutonidānā,",
  "snp4.11:3.4_0": "Ye samparāyāya narassa honti”.",
  "snp4.11:4.1_0": "“Chandānidānāni piyāni loke,",
  "snp4.11:4.2_0": "Ye cāpi lobhā vicaranti loke;",
  "snp4.11:4.3_0": "Āsā ca niṭṭhā ca itonidānā,",
  "snp4.11:4.4_0": "Ye samparāyāya narassa honti”.",
  "snp4.11:5.1_0": "“Chando nu lokasmiṁ kutonidāno,",
  "snp4.11:5.2_0": "Vinicchayā cāpi kutopahūtā;",
  "snp4.11:5.3_0": "Kodho mosavajjañca kathaṅkathā ca,",
  "snp4.11:5.4_0": "Ye vāpi dhammā samaṇena vuttā”.",
  "snp4.11:6.1_0": "“Sātaṁ asātanti yamāhu loke,",
  "snp4.11:6.2_0": "Tamūpanissāya pahoti chando;",
  "snp4.11:6.3_0": "Rūpesu disvā vibhavaṁ bhavañca,",
  "snp4.11:6.4_0": "Vinicchayaṁ kubbati jantu loke.",
  "snp4.11:7.1_0": "Kodho mosavajjañca kathaṅkathā ca,",
  "snp4.11:7.2_0": "Etepi dhammā dvayameva sante;",
  "snp4.11:7.3_0": "Kathaṅkathī ñāṇapathāya sikkhe,",
  "snp4.11:7.4_0": "Ñatvā pavuttā samaṇena dhammā”.",
  "snp4.11:8.1_0": "“Sātaṁ asātañca kutonidānā,",
  "snp4.11:8.2_0": "Kismiṁ asante na bhavanti hete;",
  "snp4.11:8.3_0": "Vibhavaṁ bhavañcāpi yametamatthaṁ,",
  "snp4.11:8.4_0": "Etaṁ me pabrūhi yatonidānaṁ”.",
  "snp4.11:9.1_0": "“Phassanidānaṁ sātaṁ asātaṁ,",
  "snp4.11:9.2_0": "Phasse asante na bhavanti hete;",
  "snp4.11:9.3_0": "Vibhavaṁ bhavañcāpi yametamatthaṁ,",
  "snp4.11:9.4_0": "Etaṁ te pabrūmi itonidānaṁ”.",
  "snp4.11:10.1_0": "“Phasso nu lokasmi kutonidāno,",
  "snp4.11:10.2_0": "Pariggahā cāpi kutopahūtā;",
  "snp4.11:10.3_0": "Kismiṁ asante na mamattamatthi,",
  "snp4.11:10.4_0": "Kismiṁ vibhūte na phusanti phassā”.",
  "snp4.11:11.1_0": "“Nāmañca rūpañca paṭicca phasso,",
  "snp4.11:11.2_0": "Icchānidānāni pariggahāni;",
  "snp4.11:11.3_0": "Icchāyasantyā na mamattamatthi,",
  "snp4.11:11.4_0": "Rūpe vibhūte na phusanti phassā”.",
  "snp4.11:12.1_0": "“Kathaṁ sametassa vibhoti rūpaṁ,",
  "snp4.11:12.2_0": "Sukhaṁ dukhañcāpi kathaṁ vibhoti;",
  "snp4.11:12.3_0": "Etaṁ me pabrūhi yathā vibhoti,",
  "snp4.11:12.4_0": "Taṁ jāniyāmāti me mano ahu”.",
  "snp4.11:13.1_0": "“Na saññasaññī na visaññasaññī,",
  "snp4.11:13.2_0": "Nopi asaññī na vibhūtasaññī;",
  "snp4.11:13.3_0": "Evaṁ sametassa vibhoti rūpaṁ,",
  "snp4.11:13.4_0": "Saññānidānā hi papañcasaṅkhā”.",
  "snp4.11:14.1_0": "“Yaṁ taṁ apucchimha akittayī no,",
  "snp4.11:14.2_0": "Aññaṁ taṁ pucchāma tadiṅgha brūhi;",
  "snp4.11:14.3_0": "Ettāvataggaṁ nu vadanti heke,",
  "snp4.11:14.4_0": "Yakkhassa suddhiṁ idha paṇḍitāse;",
  "snp4.11:14.5_0": "Udāhu aññampi vadanti etto”.",
  "snp4.11:15.1_0": "“Ettāvataggampi vadanti heke,",
  "snp4.11:15.2_0": "Yakkhassa suddhiṁ idha paṇḍitāse;",
  "snp4.11:15.3_0": "Tesaṁ paneke samayaṁ vadanti,",
  "snp4.11:15.4_0": "Anupādisese kusalā vadānā.",
  "snp4.11:16.1_0": "Ete ca ñatvā upanissitāti,",
  "snp4.11:16.2_0": "Ñatvā munī nissaye so vimaṁsī;",
  "snp4.11:16.3_0": "Ñatvā vimutto na vivādameti,",
  "snp4.11:16.4_0": "Bhavābhavāya na sameti dhīro”ti.",
  "snp4.11:16.5_0": "Kalahavivādasuttaṁ ekādasamaṁ."
}