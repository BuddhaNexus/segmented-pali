{
  "ja427:0.1_0": "Jātaka",
  "ja427:0.2_0": "Navakanipāta",
  "ja427:0.3_0": "Gijjhavagga",
  "ja427:0.4_0": "1. Gijjhajātaka",
  "ja427:1.1_0": "“Parisaṅkupatho nāma,",
  "ja427:1.2_0": "gijjhapantho sanantano;",
  "ja427:1.3_0": "Tatrāsi mātāpitaro,",
  "ja427:1.4_0": "gijjho posesi jiṇṇake;",
  "ja427:1.5_0": "Tesaṁ ajagaramedaṁ,",
  "ja427:1.6_0": "accahāsi bahuttaso.",
  "ja427:2.1_0": "Pitā ca puttaṁ avaca,",
  "ja427:2.2_0": "Jānaṁ uccaṁ papātinaṁ;",
  "ja427:2.3_0": "Supattaṁ thāmasampannaṁ,",
  "ja427:2.4_0": "Tejassiṁ dūragāminaṁ.",
  "ja427:3.1_0": "Pariplavantaṁ pathaviṁ,",
  "ja427:3.2_0": "yadā tāta vijānahi;",
  "ja427:3.3_0": "Sāgarena parikkhittaṁ,",
  "ja427:3.4_0": "cakkaṁva parimaṇḍalaṁ;",
  "ja427:3.5_0": "Tato tāta nivattassu,",
  "ja427:3.6_0": "māssu etto paraṁ gami.",
  "ja427:4.1_0": "Udapattosi vegena,",
  "ja427:4.2_0": "balī pakkhī dijuttamo;",
  "ja427:4.3_0": "Olokayanto vakkaṅgo,",
  "ja427:4.4_0": "pabbatāni vanāni ca.",
  "ja427:5.1_0": "Addassa pathaviṁ gijjho,",
  "ja427:5.2_0": "yathāsāsi pitussutaṁ;",
  "ja427:5.3_0": "Sāgarena parikkhittaṁ,",
  "ja427:5.4_0": "cakkaṁva parimaṇḍalaṁ.",
  "ja427:6.1_0": "Tañca so samatikkamma,",
  "ja427:6.2_0": "paramevaccavattatha;",
  "ja427:6.3_0": "Tañca vātasikhā tikkhā,",
  "ja427:6.4_0": "accahāsi baliṁ dijaṁ.",
  "ja427:7.1_0": "Nāsakkhātigato poso,",
  "ja427:7.2_0": "punadeva nivattituṁ;",
  "ja427:7.3_0": "Dijo byasanamāpādi,",
  "ja427:7.4_0": "verambhānaṁ vasaṁ gato.",
  "ja427:8.1_0": "Tassa puttā ca dārā ca,",
  "ja427:8.2_0": "ye caññe anujīvino;",
  "ja427:8.3_0": "Sabbe byasanamāpāduṁ,",
  "ja427:8.4_0": "anovādakare dije.",
  "ja427:9.1_0": "Evampi idha vuḍḍhānaṁ,",
  "ja427:9.2_0": "yo vākyaṁ nāvabujjhati;",
  "ja427:9.3_0": "Atisīmacaro ditto,",
  "ja427:9.4_0": "gijjhovātītasāsano;",
  "ja427:9.5_0": "Sa ve byasanaṁ pappoti,",
  "ja427:9.6_0": "akatvā vuḍḍhasāsanan”ti.",
  "ja427:10.1_0": "Gijjhajātakaṁ paṭhamaṁ."
}