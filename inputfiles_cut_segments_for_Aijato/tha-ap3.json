{
  "tha-ap3:0.1_0": "Therāpadāna",
  "tha-ap3:0.2_0": "Buddhavagga",
  "tha-ap3:0.3_0": "3. Sāriputtattheraapadāna",
  "tha-ap3:1.1_0": "Atha therāpadānaṁ suṇātha—",
  "tha-ap3:2.1_0": "“Himavantassa avidūre,",
  "tha-ap3:2.2_0": "lambako nāma pabbato;",
  "tha-ap3:2.3_0": "Assamo sukato mayhaṁ,",
  "tha-ap3:2.4_0": "paṇṇasālā sumāpitā.",
  "tha-ap3:3.1_0": "Uttānakūlā nadikā,",
  "tha-ap3:3.2_0": "supatitthā manoramā;",
  "tha-ap3:3.3_0": "Susuddhapulinākiṇṇā,",
  "tha-ap3:3.4_0": "avidūre mamassamaṁ.",
  "tha-ap3:4.1_0": "Asakkharā apabbhārā,",
  "tha-ap3:4.2_0": "sādu appaṭigandhikā;",
  "tha-ap3:4.3_0": "Sandatī nadikā tattha,",
  "tha-ap3:4.4_0": "sobhayantā mamassamaṁ.",
  "tha-ap3:5.1_0": "Kumbhīlā makarā cettha,",
  "tha-ap3:5.2_0": "susumārā ca kacchapā;",
  "tha-ap3:5.3_0": "Caranti nadiyā tattha,",
  "tha-ap3:5.4_0": "sobhayantā mamassamaṁ.",
  "tha-ap3:6.1_0": "Pāṭhīnā pāvusā macchā,",
  "tha-ap3:6.2_0": "balajā muñjarohitā;",
  "tha-ap3:6.3_0": "Vaggaḷā papatāyantā,",
  "tha-ap3:6.4_0": "sobhayanti mamassamaṁ.",
  "tha-ap3:7.1_0": "Ubho kūlesu nadiyā,",
  "tha-ap3:7.2_0": "pupphino phalino dumā;",
  "tha-ap3:7.3_0": "Ubhato abhilambantā,",
  "tha-ap3:7.4_0": "sobhayanti mamassamaṁ.",
  "tha-ap3:8.1_0": "Ambā sālā ca tilakā,",
  "tha-ap3:8.2_0": "pāṭalī sinduvārakā;",
  "tha-ap3:8.3_0": "Dibbagandhā sampavanti,",
  "tha-ap3:8.4_0": "pupphitā mama assame.",
  "tha-ap3:9.1_0": "Campakā saḷalā nīpā,",
  "tha-ap3:9.2_0": "nāgapunnāgaketakā;",
  "tha-ap3:9.3_0": "Dibbagandhā sampavanti,",
  "tha-ap3:9.4_0": "pupphitā mama assame.",
  "tha-ap3:10.1_0": "Atimuttā asokā ca,",
  "tha-ap3:10.2_0": "bhaginīmālā ca pupphitā;",
  "tha-ap3:10.3_0": "Aṅkolā bimbijālā ca,",
  "tha-ap3:10.4_0": "pupphitā mama assame.",
  "tha-ap3:11.1_0": "Ketakā kandali ceva,",
  "tha-ap3:11.2_0": "godhukā tiṇasūlikā;",
  "tha-ap3:11.3_0": "Dibbagandhaṁ sampavantā,",
  "tha-ap3:11.4_0": "sobhayanti mamassamaṁ.",
  "tha-ap3:12.1_0": "Kaṇikārā kaṇṇikā ca,",
  "tha-ap3:12.2_0": "asanā ajjunā bahū;",
  "tha-ap3:12.3_0": "Dibbagandhaṁ sampavantā,",
  "tha-ap3:12.4_0": "sobhayanti mamassamaṁ.",
  "tha-ap3:13.1_0": "Punnāgā giripunnāgā,",
  "tha-ap3:13.2_0": "koviḷārā ca pupphitā;",
  "tha-ap3:13.3_0": "Dibbagandhaṁ sampavantā,",
  "tha-ap3:13.4_0": "sobhayanti mamassamaṁ.",
  "tha-ap3:14.1_0": "Uddhālakā ca kuṭajā,",
  "tha-ap3:14.2_0": "kadambā vakulā bahū;",
  "tha-ap3:14.3_0": "Dibbagandhaṁ sampavantā,",
  "tha-ap3:14.4_0": "sobhayanti mamassamaṁ.",
  "tha-ap3:15.1_0": "Āḷakā isimuggā ca,",
  "tha-ap3:15.2_0": "kadalimātuluṅgiyo;",
  "tha-ap3:15.3_0": "Gandhodakena saṁvaḍḍhā,",
  "tha-ap3:15.4_0": "phalāni dhārayanti te.",
  "tha-ap3:16.1_0": "Aññe pupphanti padumā,",
  "tha-ap3:16.2_0": "aññe jāyanti kesarī;",
  "tha-ap3:16.3_0": "Aññe opupphā padumā,",
  "tha-ap3:16.4_0": "pupphitā taḷāke tadā.",
  "tha-ap3:17.1_0": "Gabbhaṁ gaṇhanti padumā,",
  "tha-ap3:17.2_0": "niddhāvanti mulāḷiyo;",
  "tha-ap3:17.3_0": "Siṅghāṭipattamākiṇṇā,",
  "tha-ap3:17.4_0": "sobhanti taḷāke tadā.",
  "tha-ap3:18.1_0": "Nayitā ambagandhī ca,",
  "tha-ap3:18.2_0": "uttalī bandhujīvakā;",
  "tha-ap3:18.3_0": "Dibbagandhā sampavanti,",
  "tha-ap3:18.4_0": "pupphitā taḷāke tadā.",
  "tha-ap3:19.1_0": "Pāṭhīnā pāvusā macchā,",
  "tha-ap3:19.2_0": "balajā muñjarohitā;",
  "tha-ap3:19.3_0": "Saṅgulā maggurā ceva,",
  "tha-ap3:19.4_0": "vasanti taḷāke tadā.",
  "tha-ap3:20.1_0": "Kumbhīlā susumārā ca,",
  "tha-ap3:20.2_0": "tantigāhā ca rakkhasā;",
  "tha-ap3:20.3_0": "Oguhā ajagarā ca,",
  "tha-ap3:20.4_0": "vasanti taḷāke tadā.",
  "tha-ap3:21.1_0": "Pārevatā ravihaṁsā,",
  "tha-ap3:21.2_0": "cakkavākā nadīcarā;",
  "tha-ap3:21.3_0": "Kokilā sukasāḷikā,",
  "tha-ap3:21.4_0": "upajīvanti taṁ saraṁ.",
  "tha-ap3:22.1_0": "Kukkutthakā kuḷīrakā,",
  "tha-ap3:22.2_0": "vane pokkharasātakā;",
  "tha-ap3:22.3_0": "Dindibhā suvapotā ca,",
  "tha-ap3:22.4_0": "upajīvanti taṁ saraṁ.",
  "tha-ap3:23.1_0": "Haṁsā koñcā mayūrā ca,",
  "tha-ap3:23.2_0": "kokilā tambacūḷakā;",
  "tha-ap3:23.3_0": "Pammakā jīvaṁjīvā ca,",
  "tha-ap3:23.4_0": "upajīvanti taṁ saraṁ.",
  "tha-ap3:24.1_0": "Kosikā poṭṭhasīsā ca,",
  "tha-ap3:24.2_0": "kurarā senakā bahū;",
  "tha-ap3:24.3_0": "Mahākāḷā ca sakuṇā,",
  "tha-ap3:24.4_0": "upajīvanti taṁ saraṁ.",
  "tha-ap3:25.1_0": "Pasadā ca varāhā ca,",
  "tha-ap3:25.2_0": "camarā gaṇḍakā bahū;",
  "tha-ap3:25.3_0": "Rohiccā sukapotā ca,",
  "tha-ap3:25.4_0": "upajīvanti taṁ saraṁ.",
  "tha-ap3:26.1_0": "Sīhabyagghā ca dīpī ca,",
  "tha-ap3:26.2_0": "acchakokataracchakā;",
  "tha-ap3:26.3_0": "Tidhā pabhinnamātaṅgā,",
  "tha-ap3:26.4_0": "upajīvanti taṁ saraṁ.",
  "tha-ap3:27.1_0": "Kinnarā vānarā ceva,",
  "tha-ap3:27.2_0": "athopi vanakammikā;",
  "tha-ap3:27.3_0": "Cetā ca luddakā ceva,",
  "tha-ap3:27.4_0": "upajīvanti taṁ saraṁ.",
  "tha-ap3:28.1_0": "Tindukāni piyālāni,",
  "tha-ap3:28.2_0": "madhuke kāsumāriyo;",
  "tha-ap3:28.3_0": "Dhuvaṁ phalāni dhārenti,",
  "tha-ap3:28.4_0": "avidūre mamassamaṁ.",
  "tha-ap3:29.1_0": "Kosambā saḷalā nimbā,",
  "tha-ap3:29.2_0": "sāduphalasamāyutā;",
  "tha-ap3:29.3_0": "Dhuvaṁ phalāni dhārenti,",
  "tha-ap3:29.4_0": "avidūre mamassamaṁ.",
  "tha-ap3:30.1_0": "Harītakā āmalakā,",
  "tha-ap3:30.2_0": "ambajambuvibhītakā;",
  "tha-ap3:30.3_0": "Kolā bhallātakā billā,",
  "tha-ap3:30.4_0": "phalāni dhārayanti te.",
  "tha-ap3:31.1_0": "Āluvā ca kaḷambā ca,",
  "tha-ap3:31.2_0": "biḷālītakkaḷāni ca;",
  "tha-ap3:31.3_0": "Jīvakā sutakā ceva,",
  "tha-ap3:31.4_0": "bahukā mama assame.",
  "tha-ap3:32.1_0": "Assamassāvidūramhi,",
  "tha-ap3:32.2_0": "taḷākāsuṁ sunimmitā;",
  "tha-ap3:32.3_0": "Acchodakā sītajalā,",
  "tha-ap3:32.4_0": "supatitthā manoramā.",
  "tha-ap3:33.1_0": "Padumuppalasañchannā,",
  "tha-ap3:33.2_0": "puṇḍarīkasamāyutā;",
  "tha-ap3:33.3_0": "Mandālakehi sañchannā,",
  "tha-ap3:33.4_0": "dibbagandho pavāyati.",
  "tha-ap3:34.1_0": "Evaṁ sabbaṅgasampanne,",
  "tha-ap3:34.2_0": "pupphite phalite vane;",
  "tha-ap3:34.3_0": "Sukate assame ramme,",
  "tha-ap3:34.4_0": "viharāmi ahaṁ tadā.",
  "tha-ap3:35.1_0": "Sīlavā vatasampanno,",
  "tha-ap3:35.2_0": "jhāyī jhānarato sadā;",
  "tha-ap3:35.3_0": "Pañcābhiññābalappatto,",
  "tha-ap3:35.4_0": "suruci nāma tāpaso.",
  "tha-ap3:36.1_0": "Catuvīsasahassāni,",
  "tha-ap3:36.2_0": "sissā mayhaṁ upaṭṭhahuṁ;",
  "tha-ap3:36.3_0": "Sabbeva brāhmaṇā ete,",
  "tha-ap3:36.4_0": "jātimanto yasassino.",
  "tha-ap3:37.1_0": "Lakkhaṇe itihāse ca,",
  "tha-ap3:37.2_0": "sanighaṇṭusakeṭubhe;",
  "tha-ap3:37.3_0": "Padakā veyyākaraṇā,",
  "tha-ap3:37.4_0": "sadhamme pāramiṁ gatā.",
  "tha-ap3:38.1_0": "Uppātesu nimittesu,",
  "tha-ap3:38.2_0": "lakkhaṇesu ca kovidā;",
  "tha-ap3:38.3_0": "Pathabyā bhūmantalikkhe,",
  "tha-ap3:38.4_0": "mama sissā susikkhitā.",
  "tha-ap3:39.1_0": "Appicchā nipakā ete,",
  "tha-ap3:39.2_0": "appāhārā alolupā;",
  "tha-ap3:39.3_0": "Lābhālābhena santuṭṭhā,",
  "tha-ap3:39.4_0": "parivārenti maṁ sadā.",
  "tha-ap3:40.1_0": "Jhāyī jhānaratā dhīrā,",
  "tha-ap3:40.2_0": "santacittā samāhitā;",
  "tha-ap3:40.3_0": "Ākiñcaññaṁ patthayantā,",
  "tha-ap3:40.4_0": "parivārenti maṁ sadā.",
  "tha-ap3:41.1_0": "Abhiññāpāramippattā,",
  "tha-ap3:41.2_0": "pettike gocare ratā;",
  "tha-ap3:41.3_0": "Antalikkhacarā dhīrā,",
  "tha-ap3:41.4_0": "parivārenti maṁ sadā.",
  "tha-ap3:42.1_0": "Saṁvutā chasu dvāresu,",
  "tha-ap3:42.2_0": "anejā rakkhitindriyā;",
  "tha-ap3:42.3_0": "Asaṁsaṭṭhā ca te dhīrā,",
  "tha-ap3:42.4_0": "mama sissā durāsadā.",
  "tha-ap3:43.1_0": "Pallaṅkena nisajjāya,",
  "tha-ap3:43.2_0": "ṭhānacaṅkamanena ca;",
  "tha-ap3:43.3_0": "Vītināmenti te rattiṁ,",
  "tha-ap3:43.4_0": "mama sissā durāsadā.",
  "tha-ap3:44.1_0": "Rajanīye na rajjanti,",
  "tha-ap3:44.2_0": "dussanīye na dussare;",
  "tha-ap3:44.3_0": "Mohanīye na muyhanti,",
  "tha-ap3:44.4_0": "mama sissā durāsadā.",
  "tha-ap3:45.1_0": "Iddhiṁ vīmaṁsamānā te,",
  "tha-ap3:45.2_0": "vattanti niccakālikaṁ;",
  "tha-ap3:45.3_0": "Pathaviṁ te pakampenti,",
  "tha-ap3:45.4_0": "sārambhena durāsadā.",
  "tha-ap3:46.1_0": "Kīḷamānā ca te sissā,",
  "tha-ap3:46.2_0": "kīḷanti jhānakīḷitaṁ;",
  "tha-ap3:46.3_0": "Jambuto phalamānenti,",
  "tha-ap3:46.4_0": "mama sissā durāsadā.",
  "tha-ap3:47.1_0": "Aññe gacchanti goyānaṁ,",
  "tha-ap3:47.2_0": "aññe pubbavidehakaṁ;",
  "tha-ap3:47.3_0": "Aññe ca uttarakuruṁ,",
  "tha-ap3:47.4_0": "esanāya durāsadā.",
  "tha-ap3:48.1_0": "Purato pesenti khāriṁ,",
  "tha-ap3:48.2_0": "pacchato ca vajanti te;",
  "tha-ap3:48.3_0": "Catuvīsasahassehi,",
  "tha-ap3:48.4_0": "chāditaṁ hoti ambaraṁ.",
  "tha-ap3:49.1_0": "Aggipākī anaggī ca,",
  "tha-ap3:49.2_0": "dantodukkhalikāpi ca;",
  "tha-ap3:49.3_0": "Asmena koṭṭitā keci,",
  "tha-ap3:49.4_0": "pavattaphalabhojanā.",
  "tha-ap3:50.1_0": "Udakorohaṇā keci,",
  "tha-ap3:50.2_0": "sāyaṁ pāto sucīratā;",
  "tha-ap3:50.3_0": "Toyābhisecanakarā,",
  "tha-ap3:50.4_0": "mama sissā durāsadā.",
  "tha-ap3:51.1_0": "Parūḷhakacchanakhalomā,",
  "tha-ap3:51.2_0": "paṅkadantā rajassirā;",
  "tha-ap3:51.3_0": "Gandhitā sīlagandhena,",
  "tha-ap3:51.4_0": "mama sissā durāsadā.",
  "tha-ap3:52.1_0": "Pātova sannipatitvā,",
  "tha-ap3:52.2_0": "jaṭilā uggatāpanā;",
  "tha-ap3:52.3_0": "Lābhālābhaṁ pakittetvā,",
  "tha-ap3:52.4_0": "gacchanti ambare tadā.",
  "tha-ap3:53.1_0": "Etesaṁ pakkamantānaṁ,",
  "tha-ap3:53.2_0": "mahāsaddo pavattati;",
  "tha-ap3:53.3_0": "Ajinacammasaddena,",
  "tha-ap3:53.4_0": "muditā honti devatā.",
  "tha-ap3:54.1_0": "Disodisaṁ pakkamanti,",
  "tha-ap3:54.2_0": "antalikkhacarā isī;",
  "tha-ap3:54.3_0": "Sake balenupatthaddhā,",
  "tha-ap3:54.4_0": "te gacchanti yadicchakaṁ.",
  "tha-ap3:55.1_0": "Pathavīkampakā ete,",
  "tha-ap3:55.2_0": "sabbeva nabhacārino;",
  "tha-ap3:55.3_0": "Uggatejā duppasahā,",
  "tha-ap3:55.4_0": "sāgarova akhobhiyā.",
  "tha-ap3:56.1_0": "Ṭhānacaṅkamino keci,",
  "tha-ap3:56.2_0": "keci nesajjikā isī;",
  "tha-ap3:56.3_0": "Pavattabhojanā keci,",
  "tha-ap3:56.4_0": "mama sissā durāsadā.",
  "tha-ap3:57.1_0": "Mettāvihārino ete,",
  "tha-ap3:57.2_0": "hitesī sabbapāṇinaṁ;",
  "tha-ap3:57.3_0": "Anattukkaṁsakā sabbe,",
  "tha-ap3:57.4_0": "na te vambhenti kassaci.",
  "tha-ap3:58.1_0": "Sīharājāvasambhītā,",
  "tha-ap3:58.2_0": "gajarājāva thāmavā;",
  "tha-ap3:58.3_0": "Durāsadā byagghāriva,",
  "tha-ap3:58.4_0": "āgacchanti mamantike.",
  "tha-ap3:59.1_0": "Vijjādharā devatā ca,",
  "tha-ap3:59.2_0": "nāgagandhabbarakkhasā;",
  "tha-ap3:59.3_0": "Kumbhaṇḍā dānavā garuḷā,",
  "tha-ap3:59.4_0": "upajīvanti taṁ saraṁ.",
  "tha-ap3:60.1_0": "Te jaṭākhāribharitā,",
  "tha-ap3:60.2_0": "ajinuttaravāsanā;",
  "tha-ap3:60.3_0": "Antalikkhacarā sabbe,",
  "tha-ap3:60.4_0": "upajīvanti taṁ saraṁ.",
  "tha-ap3:61.1_0": "Sadānucchavikā ete,",
  "tha-ap3:61.2_0": "aññamaññaṁ sagāravā;",
  "tha-ap3:61.3_0": "Catubbīsasahassānaṁ,",
  "tha-ap3:61.4_0": "khipitasaddo na vijjati.",
  "tha-ap3:62.1_0": "Pāde pādaṁ nikkhipantā,",
  "tha-ap3:62.2_0": "appasaddā susaṁvutā;",
  "tha-ap3:62.3_0": "Upasaṅkamma sabbeva,",
  "tha-ap3:62.4_0": "sirasā vandare mamaṁ.",
  "tha-ap3:63.1_0": "Tehi sissehi parivuto,",
  "tha-ap3:63.2_0": "santehi ca tapassibhi;",
  "tha-ap3:63.3_0": "Vasāmi assame tattha,",
  "tha-ap3:63.4_0": "jhāyī jhānarato ahaṁ.",
  "tha-ap3:64.1_0": "Isīnaṁ sīlagandhena,",
  "tha-ap3:64.2_0": "pupphagandhena cūbhayaṁ;",
  "tha-ap3:64.3_0": "Phalīnaṁ phalagandhena,",
  "tha-ap3:64.4_0": "gandhito hoti assamo.",
  "tha-ap3:65.1_0": "Rattindivaṁ na jānāmi,",
  "tha-ap3:65.2_0": "arati me na vijjati;",
  "tha-ap3:65.3_0": "Sake sisse ovadanto,",
  "tha-ap3:65.4_0": "bhiyyo hāsaṁ labhāmahaṁ.",
  "tha-ap3:66.1_0": "Pupphānaṁ pupphamānānaṁ,",
  "tha-ap3:66.2_0": "phalānañca vipaccataṁ;",
  "tha-ap3:66.3_0": "Dibbagandhā pavāyanti,",
  "tha-ap3:66.4_0": "sobhayantā mamassamaṁ.",
  "tha-ap3:67.1_0": "Samādhimhā vuṭṭhahitvā,",
  "tha-ap3:67.2_0": "ātāpī nipako ahaṁ;",
  "tha-ap3:67.3_0": "Khāribhāraṁ gahetvāna,",
  "tha-ap3:67.4_0": "vanaṁ ajjhogahiṁ ahaṁ.",
  "tha-ap3:68.1_0": "Uppāte supine cāpi,",
  "tha-ap3:68.2_0": "Lakkhaṇesu susikkhito;",
  "tha-ap3:68.3_0": "Pavattamānaṁ mantapadaṁ,",
  "tha-ap3:68.4_0": "Dhārayāmi ahaṁ tadā.",
  "tha-ap3:69.1_0": "Anomadassī bhagavā,",
  "tha-ap3:69.2_0": "lokajeṭṭho narāsabho;",
  "tha-ap3:69.3_0": "Vivekakāmo sambuddho,",
  "tha-ap3:69.4_0": "himavantamupāgami.",
  "tha-ap3:70.1_0": "Ajjhogāhetvā himavantaṁ,",
  "tha-ap3:70.2_0": "aggo kāruṇiko muni;",
  "tha-ap3:70.3_0": "Pallaṅkaṁ ābhujitvāna,",
  "tha-ap3:70.4_0": "nisīdi purisuttamo.",
  "tha-ap3:71.1_0": "Tamaddasāhaṁ sambuddhaṁ,",
  "tha-ap3:71.2_0": "sappabhāsaṁ manoramaṁ;",
  "tha-ap3:71.3_0": "Indīvaraṁva jalitaṁ,",
  "tha-ap3:71.4_0": "ādittaṁva hutāsanaṁ.",
  "tha-ap3:72.1_0": "Jalantaṁ dīparukkhaṁva,",
  "tha-ap3:72.2_0": "vijjutaṁ gagane yathā;",
  "tha-ap3:72.3_0": "Suphullaṁ sālarājaṁva,",
  "tha-ap3:72.4_0": "addasaṁ lokanāyakaṁ.",
  "tha-ap3:73.1_0": "Ayaṁ nāgo mahāvīro,",
  "tha-ap3:73.2_0": "dukkhassantakaro muni;",
  "tha-ap3:73.3_0": "Imaṁ dassanamāgamma,",
  "tha-ap3:73.4_0": "sabbadukkhā pamuccare.",
  "tha-ap3:74.1_0": "Disvānāhaṁ devadevaṁ,",
  "tha-ap3:74.2_0": "lakkhaṇaṁ upadhārayiṁ;",
  "tha-ap3:74.3_0": "Buddho nu kho na vā buddho,",
  "tha-ap3:74.4_0": "handa passāmi cakkhumaṁ.",
  "tha-ap3:75.1_0": "Sahassārāni cakkāni,",
  "tha-ap3:75.2_0": "dissanti caraṇuttame;",
  "tha-ap3:75.3_0": "Lakkhaṇānissa disvāna,",
  "tha-ap3:75.4_0": "niṭṭhaṁ gacchiṁ tathāgate.",
  "tha-ap3:76.1_0": "Sammajjaniṁ gahetvāna,",
  "tha-ap3:76.2_0": "sammajjitvānahaṁ tadā;",
  "tha-ap3:76.3_0": "Atha pupphe samānetvā,",
  "tha-ap3:76.4_0": "buddhaseṭṭhaṁ apūjayiṁ.",
  "tha-ap3:77.1_0": "Pūjayitvāna taṁ buddhaṁ,",
  "tha-ap3:77.2_0": "oghatiṇṇamanāsavaṁ;",
  "tha-ap3:77.3_0": "Ekaṁsaṁ ajinaṁ katvā,",
  "tha-ap3:77.4_0": "namassiṁ lokanāyakaṁ.",
  "tha-ap3:78.1_0": "Yena ñāṇena sambuddho,",
  "tha-ap3:78.2_0": "viharati anāsavo;",
  "tha-ap3:78.3_0": "Taṁ ñāṇaṁ kittayissāmi,",
  "tha-ap3:78.4_0": "suṇātha mama bhāsato.",
  "tha-ap3:79.1_0": "‘Samuddharasimaṁ lokaṁ,",
  "tha-ap3:79.2_0": "sayambhū amitodaya;",
  "tha-ap3:79.3_0": "Tava dassanamāgamma,",
  "tha-ap3:79.4_0": "kaṅkhāsotaṁ taranti te.",
  "tha-ap3:80.1_0": "Tuvaṁ satthā ca ketu ca,",
  "tha-ap3:80.2_0": "dhajo yūpo ca pāṇinaṁ;",
  "tha-ap3:80.3_0": "Parāyaṇo patiṭṭhā ca,",
  "tha-ap3:80.4_0": "dīpo ca dvipaduttamo.",
  "tha-ap3:81.1_0": "Sakkā samudde udakaṁ,",
  "tha-ap3:81.2_0": "pametuṁ āḷhakena vā;",
  "tha-ap3:81.3_0": "Na tveva tava sabbaññu,",
  "tha-ap3:81.4_0": "ñāṇaṁ sakkā pametave.",
  "tha-ap3:82.1_0": "Dhāretuṁ pathaviṁ sakkā,",
  "tha-ap3:82.2_0": "ṭhapetvā tulamaṇḍale;",
  "tha-ap3:82.3_0": "Na tveva tava sabbaññu,",
  "tha-ap3:82.4_0": "ñāṇaṁ sakkā dharetave.",
  "tha-ap3:83.1_0": "Ākāso minituṁ sakkā,",
  "tha-ap3:83.2_0": "rajjuyā aṅgulena vā;",
  "tha-ap3:83.3_0": "Na tveva tava sabbaññu,",
  "tha-ap3:83.4_0": "ñāṇaṁ sakkā pametave.",
  "tha-ap3:84.1_0": "Mahāsamudde udakaṁ,",
  "tha-ap3:84.2_0": "pathavī cākhilājaṭaṁ;",
  "tha-ap3:84.3_0": "Buddhañāṇaṁ upādāya,",
  "tha-ap3:84.4_0": "upamāto na yujjare.",
  "tha-ap3:85.1_0": "Sadevakassa lokassa,",
  "tha-ap3:85.2_0": "cittaṁ yesaṁ pavattati;",
  "tha-ap3:85.3_0": "Antojālagatā ete,",
  "tha-ap3:85.4_0": "tava ñāṇamhi cakkhuma.",
  "tha-ap3:86.1_0": "Yena ñāṇena pattosi,",
  "tha-ap3:86.2_0": "kevalaṁ bodhimuttamaṁ;",
  "tha-ap3:86.3_0": "Tena ñāṇena sabbaññu,",
  "tha-ap3:86.4_0": "maddasī paratitthiye’.",
  "tha-ap3:87.1_0": "Imā gāthā thavitvāna,",
  "tha-ap3:87.2_0": "suruci nāma tāpaso;",
  "tha-ap3:87.3_0": "Ajinaṁ pattharitvāna,",
  "tha-ap3:87.4_0": "pathaviyaṁ nisīdi so.",
  "tha-ap3:88.1_0": "‘Cullāsītisahassāni,",
  "tha-ap3:88.2_0": "ajjhogāḷho mahaṇṇave;",
  "tha-ap3:88.3_0": "Accuggato tāvadeva,",
  "tha-ap3:88.4_0": "girirājā pavuccati.",
  "tha-ap3:89.1_0": "Tāva accuggato neru,",
  "tha-ap3:89.2_0": "āyato vitthato ca so;",
  "tha-ap3:89.3_0": "Cuṇṇito aṇubhedena,",
  "tha-ap3:89.4_0": "koṭisatasahassaso.",
  "tha-ap3:90.1_0": "Lakkhe ṭhapiyamānamhi,",
  "tha-ap3:90.2_0": "parikkhayamagacchatha;",
  "tha-ap3:90.3_0": "Na tveva tava sabbaññu,",
  "tha-ap3:90.4_0": "ñāṇaṁ sakkā pametave.",
  "tha-ap3:91.1_0": "Sukhumacchikena jālena,",
  "tha-ap3:91.2_0": "udakaṁ yo parikkhipe;",
  "tha-ap3:91.3_0": "Ye keci udake pāṇā,",
  "tha-ap3:91.4_0": "antojālagatā siyuṁ.",
  "tha-ap3:92.1_0": "Tatheva hi mahāvīra,",
  "tha-ap3:92.2_0": "ye keci puthutitthiyā;",
  "tha-ap3:92.3_0": "Diṭṭhigahanapakkhandā,",
  "tha-ap3:92.4_0": "parāmāsena mohitā.",
  "tha-ap3:93.1_0": "Tava suddhena ñāṇena,",
  "tha-ap3:93.2_0": "anāvaraṇadassinā;",
  "tha-ap3:93.3_0": "Antojālagatā ete,",
  "tha-ap3:93.4_0": "ñāṇaṁ te nātivattare’.",
  "tha-ap3:94.1_0": "Bhagavā tamhi samaye,",
  "tha-ap3:94.2_0": "anomadassī mahāyaso;",
  "tha-ap3:94.3_0": "Vuṭṭhahitvā samādhimhā,",
  "tha-ap3:94.4_0": "disaṁ olokayī jino.",
  "tha-ap3:95.1_0": "Anomadassimunino,",
  "tha-ap3:95.2_0": "Nisabho nāma sāvako;",
  "tha-ap3:95.3_0": "Parivuto satasahassehi,",
  "tha-ap3:95.4_0": "Santacittehi tādibhi.",
  "tha-ap3:96.1_0": "Khīṇāsavehi suddhehi,",
  "tha-ap3:96.2_0": "chaḷabhiññehi jhāyibhi;",
  "tha-ap3:96.3_0": "Cittamaññāya buddhassa,",
  "tha-ap3:96.4_0": "upesi lokanāyakaṁ.",
  "tha-ap3:97.1_0": "Antalikkhe ṭhitā tattha,",
  "tha-ap3:97.2_0": "padakkhiṇamakaṁsu te;",
  "tha-ap3:97.3_0": "Namassantā pañjalikā,",
  "tha-ap3:97.4_0": "otaruṁ buddhasantike.",
  "tha-ap3:98.1_0": "Anomadassī bhagavā,",
  "tha-ap3:98.2_0": "lokajeṭṭho narāsabho;",
  "tha-ap3:98.3_0": "Bhikkhusaṅghe nisīditvā,",
  "tha-ap3:98.4_0": "sītaṁ pātukarī jino.",
  "tha-ap3:99.1_0": "Varuṇo nāmupaṭṭhāko,",
  "tha-ap3:99.2_0": "anomadassissa satthuno;",
  "tha-ap3:99.3_0": "Ekaṁsaṁ cīvaraṁ katvā,",
  "tha-ap3:99.4_0": "apucchi lokanāyakaṁ.",
  "tha-ap3:100.1_0": "‘Ko nu kho bhagavā hetu,",
  "tha-ap3:100.2_0": "sitakammassa satthuno;",
  "tha-ap3:100.3_0": "Na hi buddhā ahetūhi,",
  "tha-ap3:100.4_0": "sitaṁ pātukaronti te’.",
  "tha-ap3:101.1_0": "Anomadassī bhagavā,",
  "tha-ap3:101.2_0": "lokajeṭṭho narāsabho;",
  "tha-ap3:101.3_0": "Bhikkhumajjhe nisīditvā,",
  "tha-ap3:101.4_0": "imaṁ gāthaṁ abhāsatha.",
  "tha-ap3:102.1_0": "‘Yo maṁ pupphena pūjesi,",
  "tha-ap3:102.2_0": "ñāṇañcāpi anutthavi;",
  "tha-ap3:102.3_0": "Tamahaṁ kittayissāmi,",
  "tha-ap3:102.4_0": "suṇotha mama bhāsato’.",
  "tha-ap3:103.1_0": "Buddhassa giramaññāya,",
  "tha-ap3:103.2_0": "sabbe devā samāgatā;",
  "tha-ap3:103.3_0": "Saddhammaṁ sotukāmā te,",
  "tha-ap3:103.4_0": "sambuddhamupasaṅkamuṁ.",
  "tha-ap3:104.1_0": "Dasasu lokadhātūsu,",
  "tha-ap3:104.2_0": "devakāyā mahiddhikā;",
  "tha-ap3:104.3_0": "Saddhammaṁ sotukāmā te,",
  "tha-ap3:104.4_0": "sambuddhamupasaṅkamuṁ.",
  "tha-ap3:105.1_0": "‘Hatthī assā rathā pattī,",
  "tha-ap3:105.2_0": "senā ca caturaṅginī;",
  "tha-ap3:105.3_0": "Parivāressantimaṁ niccaṁ,",
  "tha-ap3:105.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap3:106.1_0": "Saṭṭhitūriyasahassāni,",
  "tha-ap3:106.2_0": "bheriyo samalaṅkatā;",
  "tha-ap3:106.3_0": "Upaṭṭhissantimaṁ niccaṁ,",
  "tha-ap3:106.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap3:107.1_0": "Soḷasitthisahassāni,",
  "tha-ap3:107.2_0": "nāriyo samalaṅkatā;",
  "tha-ap3:107.3_0": "Vicittavatthābharaṇā,",
  "tha-ap3:107.4_0": "āmuttamaṇikuṇḍalā.",
  "tha-ap3:108.1_0": "Aḷārapamhā hasulā,",
  "tha-ap3:108.2_0": "susaññā tanumajjhimā;",
  "tha-ap3:108.3_0": "Parivāressantimaṁ niccaṁ,",
  "tha-ap3:108.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap3:109.1_0": "Kappasatasahassāni,",
  "tha-ap3:109.2_0": "devaloke ramissati;",
  "tha-ap3:109.3_0": "Sahassakkhattuṁ cakkavattī,",
  "tha-ap3:109.4_0": "rājā raṭṭhe bhavissati.",
  "tha-ap3:110.1_0": "Sahassakkhattuṁ devindo,",
  "tha-ap3:110.2_0": "devarajjaṁ karissati;",
  "tha-ap3:110.3_0": "Padesarajjaṁ vipulaṁ,",
  "tha-ap3:110.4_0": "gaṇanāto asaṅkhiyaṁ.",
  "tha-ap3:111.1_0": "Pacchime bhavasampatte,",
  "tha-ap3:111.2_0": "manussattaṁ gamissati;",
  "tha-ap3:111.3_0": "Brāhmaṇī sāriyā nāma,",
  "tha-ap3:111.4_0": "dhārayissati kucchinā.",
  "tha-ap3:112.1_0": "Mātuyā nāmagottena,",
  "tha-ap3:112.2_0": "paññāyissatiyaṁ naro;",
  "tha-ap3:112.3_0": "Sāriputtoti nāmena,",
  "tha-ap3:112.4_0": "tikkhapañño bhavissati.",
  "tha-ap3:113.1_0": "Asītikoṭī chaḍḍetvā,",
  "tha-ap3:113.2_0": "pabbajissatikiñcano;",
  "tha-ap3:113.3_0": "Gavesanto santipadaṁ,",
  "tha-ap3:113.4_0": "carissati mahiṁ imaṁ.",
  "tha-ap3:114.1_0": "Apparimeyye ito kappe,",
  "tha-ap3:114.2_0": "okkākakulasambhavo;",
  "tha-ap3:114.3_0": "Gotamo nāma gottena,",
  "tha-ap3:114.4_0": "satthā loke bhavissati.",
  "tha-ap3:115.1_0": "Tassa dhammesu dāyādo,",
  "tha-ap3:115.2_0": "oraso dhammanimmito;",
  "tha-ap3:115.3_0": "Sāriputtoti nāmena,",
  "tha-ap3:115.4_0": "hessati aggasāvako.",
  "tha-ap3:116.1_0": "Ayaṁ bhāgīrathī gaṅgā,",
  "tha-ap3:116.2_0": "himavantā pabhāvitā;",
  "tha-ap3:116.3_0": "Mahāsamuddamappeti,",
  "tha-ap3:116.4_0": "tappayantī mahodadhiṁ.",
  "tha-ap3:117.1_0": "Tathevāyaṁ sāriputto,",
  "tha-ap3:117.2_0": "sake tīsu visārado;",
  "tha-ap3:117.3_0": "Paññāya pāramiṁ gantvā,",
  "tha-ap3:117.4_0": "tappayissati pāṇine.",
  "tha-ap3:118.1_0": "Himavantamupādāya,",
  "tha-ap3:118.2_0": "sāgarañca mahodadhiṁ;",
  "tha-ap3:118.3_0": "Etthantare yaṁ pulinaṁ,",
  "tha-ap3:118.4_0": "gaṇanāto asaṅkhiyaṁ.",
  "tha-ap3:119.1_0": "Tampi sakkā asesena,",
  "tha-ap3:119.2_0": "saṅkhātuṁ gaṇanā yathā;",
  "tha-ap3:119.3_0": "Na tveva sāriputtassa,",
  "tha-ap3:119.4_0": "paññāyanto bhavissati.",
  "tha-ap3:120.1_0": "Lakkhe ṭhapiyamānamhi,",
  "tha-ap3:120.2_0": "khīye gaṅgāya vālukā;",
  "tha-ap3:120.3_0": "Na tveva sāriputtassa,",
  "tha-ap3:120.4_0": "paññāyanto bhavissati.",
  "tha-ap3:121.1_0": "Mahāsamudde ūmiyo,",
  "tha-ap3:121.2_0": "gaṇanāto asaṅkhiyā;",
  "tha-ap3:121.3_0": "Tatheva sāriputtassa,",
  "tha-ap3:121.4_0": "paññāyanto na hessati.",
  "tha-ap3:122.1_0": "Ārādhayitvā sambuddhaṁ,",
  "tha-ap3:122.2_0": "gotamaṁ sakyapuṅgavaṁ;",
  "tha-ap3:122.3_0": "Paññāya pāramiṁ gantvā,",
  "tha-ap3:122.4_0": "hessati aggasāvako.",
  "tha-ap3:123.1_0": "Pavattitaṁ dhammacakkaṁ,",
  "tha-ap3:123.2_0": "sakyaputtena tādinā;",
  "tha-ap3:123.3_0": "Anuvattessati sammā,",
  "tha-ap3:123.4_0": "vassento dhammavuṭṭhiyo.",
  "tha-ap3:124.1_0": "Sabbametaṁ abhiññāya,",
  "tha-ap3:124.2_0": "gotamo sakyapuṅgavo;",
  "tha-ap3:124.3_0": "Bhikkhusaṅghe nisīditvā,",
  "tha-ap3:124.4_0": "aggaṭṭhāne ṭhapessati’.",
  "tha-ap3:125.1_0": "Aho me sukataṁ kammaṁ,",
  "tha-ap3:125.2_0": "anomadassissa satthuno;",
  "tha-ap3:125.3_0": "Yassāhaṁ kāraṁ katvāna,",
  "tha-ap3:125.4_0": "sabbattha pāramiṁ gato.",
  "tha-ap3:126.1_0": "Aparimeyye kataṁ kammaṁ,",
  "tha-ap3:126.2_0": "phalaṁ dassesi me idha;",
  "tha-ap3:126.3_0": "Sumutto saravegova,",
  "tha-ap3:126.4_0": "kilese jhāpayiṁ ahaṁ.",
  "tha-ap3:127.1_0": "Asaṅkhataṁ gavesanto,",
  "tha-ap3:127.2_0": "nibbānaṁ acalaṁ padaṁ;",
  "tha-ap3:127.3_0": "Vicinaṁ titthiye sabbe,",
  "tha-ap3:127.4_0": "esāhaṁ saṁsariṁ bhave.",
  "tha-ap3:128.1_0": "Yathāpi byādhito poso,",
  "tha-ap3:128.2_0": "pariyeseyya osadhaṁ;",
  "tha-ap3:128.3_0": "Vicineyya vanaṁ sabbaṁ,",
  "tha-ap3:128.4_0": "byādhito parimuttiyā.",
  "tha-ap3:129.1_0": "Asaṅkhataṁ gavesanto,",
  "tha-ap3:129.2_0": "nibbānaṁ amataṁ padaṁ;",
  "tha-ap3:129.3_0": "Abbokiṇṇaṁ pañcasataṁ,",
  "tha-ap3:129.4_0": "pabbajiṁ isipabbajaṁ.",
  "tha-ap3:130.1_0": "Jaṭābhārena bharito,",
  "tha-ap3:130.2_0": "ajinuttaranivāsano;",
  "tha-ap3:130.3_0": "Abhiññāpāramiṁ gantvā,",
  "tha-ap3:130.4_0": "brahmalokaṁ agacchihaṁ.",
  "tha-ap3:131.1_0": "Natthi bāhirake suddhi,",
  "tha-ap3:131.2_0": "ṭhapetvā jinasāsanaṁ;",
  "tha-ap3:131.3_0": "Ye keci buddhimā sattā,",
  "tha-ap3:131.4_0": "sujjhanti jinasāsane.",
  "tha-ap3:132.1_0": "Attakāramayaṁ etaṁ,",
  "tha-ap3:132.2_0": "nayidaṁ itihītihaṁ;",
  "tha-ap3:132.3_0": "Asaṅkhataṁ gavesanto,",
  "tha-ap3:132.4_0": "kutitthe sañcariṁ ahaṁ.",
  "tha-ap3:133.1_0": "Yathā sāratthiko poso,",
  "tha-ap3:133.2_0": "kadaliṁ chetvāna phālaye;",
  "tha-ap3:133.3_0": "Na tattha sāraṁ vindeyya,",
  "tha-ap3:133.4_0": "sārena rittako hi so.",
  "tha-ap3:134.1_0": "Tatheva titthiyā loke,",
  "tha-ap3:134.2_0": "nānādiṭṭhī bahujjanā;",
  "tha-ap3:134.3_0": "Asaṅkhatena rittāse,",
  "tha-ap3:134.4_0": "sārena kadalī yathā.",
  "tha-ap3:135.1_0": "Pacchime bhavasampatte,",
  "tha-ap3:135.2_0": "brahmabandhu ahosahaṁ;",
  "tha-ap3:135.3_0": "Mahābhogaṁ chaḍḍetvāna,",
  "tha-ap3:135.4_0": "pabbajiṁ anagāriyaṁ”.",
  "tha-ap3:136.1_0": "Paṭhamabhāṇavāraṁ.",
  "tha-ap3:137.1_0": "“Ajjhāyako mantadharo,",
  "tha-ap3:137.2_0": "tiṇṇaṁ vedāna pāragū;",
  "tha-ap3:137.3_0": "Brāhmaṇo sañcayo nāma,",
  "tha-ap3:137.4_0": "tassa mūle vasāmahaṁ.",
  "tha-ap3:138.1_0": "Sāvako te mahāvīra,",
  "tha-ap3:138.2_0": "assaji nāma brāhmaṇo;",
  "tha-ap3:138.3_0": "Durāsado uggatejo,",
  "tha-ap3:138.4_0": "piṇḍāya caratī tadā.",
  "tha-ap3:139.1_0": "Tamaddasāsiṁ sappaññaṁ,",
  "tha-ap3:139.2_0": "muniṁ mone samāhitaṁ;",
  "tha-ap3:139.3_0": "Santacittaṁ mahānāgaṁ,",
  "tha-ap3:139.4_0": "suphullaṁ padumaṁ yathā.",
  "tha-ap3:140.1_0": "Disvā me cittamuppajji,",
  "tha-ap3:140.2_0": "sudantaṁ suddhamānasaṁ;",
  "tha-ap3:140.3_0": "Usabhaṁ pavaraṁ vīraṁ,",
  "tha-ap3:140.4_0": "arahāyaṁ bhavissati.",
  "tha-ap3:141.1_0": "Pāsādiko iriyati,",
  "tha-ap3:141.2_0": "abhirūpo susaṁvuto;",
  "tha-ap3:141.3_0": "Uttame damathe danto,",
  "tha-ap3:141.4_0": "amatadassī bhavissati.",
  "tha-ap3:142.1_0": "Yannūnāhaṁ uttamatthaṁ,",
  "tha-ap3:142.2_0": "puccheyyaṁ tuṭṭhamānasaṁ;",
  "tha-ap3:142.3_0": "So me puṭṭho kathessati,",
  "tha-ap3:142.4_0": "paṭipucchāmahaṁ tadā.",
  "tha-ap3:143.1_0": "Piṇḍapātaṁ carantassa,",
  "tha-ap3:143.2_0": "pacchato agamāsahaṁ;",
  "tha-ap3:143.3_0": "Okāsaṁ paṭimānento,",
  "tha-ap3:143.4_0": "pucchituṁ amataṁ padaṁ.",
  "tha-ap3:144.1_0": "Vīthintare anuppattaṁ,",
  "tha-ap3:144.2_0": "upagantvāna pucchahaṁ;",
  "tha-ap3:144.3_0": "‘Kathaṁ gottosi tvaṁ vīra,",
  "tha-ap3:144.4_0": "kassa sissosi mārisa’.",
  "tha-ap3:145.1_0": "So me puṭṭho viyākāsi,",
  "tha-ap3:145.2_0": "asambhītova kesarī;",
  "tha-ap3:145.3_0": "‘Buddho loke samuppanno,",
  "tha-ap3:145.4_0": "tassa sissomhi āvuso’.",
  "tha-ap3:146.1_0": "‘Kīdisaṁ te mahāvīra,",
  "tha-ap3:146.2_0": "anujāta mahāyasa;",
  "tha-ap3:146.3_0": "Buddhassa sāsanaṁ dhammaṁ,",
  "tha-ap3:146.4_0": "sādhu me kathayassu bho’.",
  "tha-ap3:147.1_0": "So me puṭṭho kathī sabbaṁ,",
  "tha-ap3:147.2_0": "gambhīraṁ nipuṇaṁ padaṁ;",
  "tha-ap3:147.3_0": "Taṇhāsallassa hantāraṁ,",
  "tha-ap3:147.4_0": "sabbadukkhāpanūdanaṁ.",
  "tha-ap3:148.1_0": "‘Ye dhammā hetuppabhavā,",
  "tha-ap3:148.2_0": "Tesaṁ hetuṁ tathāgato āha;",
  "tha-ap3:148.3_0": "Tesañca yo nirodho,",
  "tha-ap3:148.4_0": "Evaṁvādī mahāsamaṇo’.",
  "tha-ap3:149.1_0": "Sohaṁ vissajjite pañhe,",
  "tha-ap3:149.2_0": "paṭhamaṁ phalamajjhagaṁ;",
  "tha-ap3:149.3_0": "Virajo vimalo āsiṁ,",
  "tha-ap3:149.4_0": "sutvāna jinasāsanaṁ.",
  "tha-ap3:150.1_0": "Sutvāna munino vākyaṁ,",
  "tha-ap3:150.2_0": "passitvā dhammamuttamaṁ;",
  "tha-ap3:150.3_0": "Pariyogāḷhasaddhammo,",
  "tha-ap3:150.4_0": "imaṁ gāthamabhāsahaṁ.",
  "tha-ap3:151.1_0": "‘Eseva dhammo yadi tāvadeva,",
  "tha-ap3:151.2_0": "Paccabyatha padamasokaṁ;",
  "tha-ap3:151.3_0": "Adiṭṭhaṁ abbhatītaṁ,",
  "tha-ap3:151.4_0": "Bahukehi kappanahutehi’.",
  "tha-ap3:152.1_0": "Svāhaṁ dhammaṁ gavesanto,",
  "tha-ap3:152.2_0": "kutitthe sañcariṁ ahaṁ;",
  "tha-ap3:152.3_0": "So me attho anuppatto,",
  "tha-ap3:152.4_0": "kālo me nappamajjituṁ.",
  "tha-ap3:153.1_0": "Tositohaṁ assajinā,",
  "tha-ap3:153.2_0": "patvāna acalaṁ padaṁ;",
  "tha-ap3:153.3_0": "Sahāyakaṁ gavesanto,",
  "tha-ap3:153.4_0": "assamaṁ agamāsahaṁ.",
  "tha-ap3:154.1_0": "Dūratova mamaṁ disvā,",
  "tha-ap3:154.2_0": "sahāyo me susikkhito;",
  "tha-ap3:154.3_0": "Iriyāpathasampanno,",
  "tha-ap3:154.4_0": "idaṁ vacanamabravi.",
  "tha-ap3:155.1_0": "‘Pasannamukhanettosi,",
  "tha-ap3:155.2_0": "munibhāvova dissati;",
  "tha-ap3:155.3_0": "Amatādhigato kacci,",
  "tha-ap3:155.4_0": "nibbānamaccutaṁ padaṁ.",
  "tha-ap3:156.1_0": "Subhānurūpo āyāsi,",
  "tha-ap3:156.2_0": "āneñjakārito viya;",
  "tha-ap3:156.3_0": "Dantova dantadamatho,",
  "tha-ap3:156.4_0": "upasantosi brāhmaṇa’.",
  "tha-ap3:157.1_0": "‘Amataṁ mayādhigataṁ,",
  "tha-ap3:157.2_0": "sokasallāpanūdanaṁ;",
  "tha-ap3:157.3_0": "Tvampi taṁ adhigacchesi,",
  "tha-ap3:157.4_0": "gacchāma buddhasantikaṁ’.",
  "tha-ap3:158.1_0": "Sādhūti so paṭissutvā,",
  "tha-ap3:158.2_0": "sahāyo me susikkhito;",
  "tha-ap3:158.3_0": "Hatthena hatthaṁ gaṇhitvā,",
  "tha-ap3:158.4_0": "upagamma tavantikaṁ.",
  "tha-ap3:159.1_0": "Ubhopi pabbajissāma,",
  "tha-ap3:159.2_0": "sakyaputta tavantike;",
  "tha-ap3:159.3_0": "Tava sāsanamāgamma,",
  "tha-ap3:159.4_0": "viharāma anāsavā.",
  "tha-ap3:160.1_0": "Kolito iddhiyā seṭṭho,",
  "tha-ap3:160.2_0": "ahaṁ paññāya pārago;",
  "tha-ap3:160.3_0": "Ubhova ekato hutvā,",
  "tha-ap3:160.4_0": "sāsanaṁ sobhayāmase.",
  "tha-ap3:161.1_0": "Apariyositasaṅkappo,",
  "tha-ap3:161.2_0": "kutitthe sañcariṁ ahaṁ;",
  "tha-ap3:161.3_0": "Tava dassanamāgamma,",
  "tha-ap3:161.4_0": "saṅkappo pūrito mama.",
  "tha-ap3:162.1_0": "Pathaviyaṁ patiṭṭhāya,",
  "tha-ap3:162.2_0": "pupphanti samaye dumā;",
  "tha-ap3:162.3_0": "Dibbagandhā sampavanti,",
  "tha-ap3:162.4_0": "tosenti sabbapāṇinaṁ.",
  "tha-ap3:163.1_0": "Tathevāhaṁ mahāvīra,",
  "tha-ap3:163.2_0": "sakyaputta mahāyasa;",
  "tha-ap3:163.3_0": "Sāsane te patiṭṭhāya,",
  "tha-ap3:163.4_0": "samayesāmi pupphituṁ.",
  "tha-ap3:164.1_0": "Vimuttipupphaṁ esanto,",
  "tha-ap3:164.2_0": "bhavasaṁsāramocanaṁ;",
  "tha-ap3:164.3_0": "Vimuttipupphalābhena,",
  "tha-ap3:164.4_0": "tosemi sabbapāṇinaṁ.",
  "tha-ap3:165.1_0": "Yāvatā buddhakhettamhi,",
  "tha-ap3:165.2_0": "ṭhapetvāna mahāmuniṁ;",
  "tha-ap3:165.3_0": "Paññāya sadiso natthi,",
  "tha-ap3:165.4_0": "tava puttassa cakkhuma.",
  "tha-ap3:166.1_0": "Suvinītā ca te sissā,",
  "tha-ap3:166.2_0": "parisā ca susikkhitā;",
  "tha-ap3:166.3_0": "Uttame damathe dantā,",
  "tha-ap3:166.4_0": "parivārenti taṁ sadā.",
  "tha-ap3:167.1_0": "Jhāyī jhānaratā dhīrā,",
  "tha-ap3:167.2_0": "santacittā samāhitā;",
  "tha-ap3:167.3_0": "Munī moneyyasampannā,",
  "tha-ap3:167.4_0": "parivārenti taṁ sadā.",
  "tha-ap3:168.1_0": "Appicchā nipakā dhīrā,",
  "tha-ap3:168.2_0": "appāhārā alolupā;",
  "tha-ap3:168.3_0": "Lābhālābhena santuṭṭhā,",
  "tha-ap3:168.4_0": "parivārenti taṁ sadā.",
  "tha-ap3:169.1_0": "Āraññikā dhutaratā,",
  "tha-ap3:169.2_0": "jhāyino lūkhacīvarā;",
  "tha-ap3:169.3_0": "Vivekābhiratā dhīrā,",
  "tha-ap3:169.4_0": "parivārenti taṁ sadā.",
  "tha-ap3:170.1_0": "Paṭipannā phalaṭṭhā ca,",
  "tha-ap3:170.2_0": "sekhā phalasamaṅgino;",
  "tha-ap3:170.3_0": "Āsīsakā uttamatthaṁ,",
  "tha-ap3:170.4_0": "parivārenti taṁ sadā.",
  "tha-ap3:171.1_0": "Sotāpannā ca vimalā,",
  "tha-ap3:171.2_0": "sakadāgāmino ca ye;",
  "tha-ap3:171.3_0": "Anāgāmī ca arahā,",
  "tha-ap3:171.4_0": "parivārenti taṁ sadā.",
  "tha-ap3:172.1_0": "Satipaṭṭhānakusalā,",
  "tha-ap3:172.2_0": "bojjhaṅgabhāvanāratā;",
  "tha-ap3:172.3_0": "Sāvakā te bahū sabbe,",
  "tha-ap3:172.4_0": "parivārenti taṁ sadā.",
  "tha-ap3:173.1_0": "Iddhipādesu kusalā,",
  "tha-ap3:173.2_0": "samādhibhāvanāratā;",
  "tha-ap3:173.3_0": "Sammappadhānānuyuttā,",
  "tha-ap3:173.4_0": "parivārenti taṁ sadā.",
  "tha-ap3:174.1_0": "Tevijjā chaḷabhiññā ca,",
  "tha-ap3:174.2_0": "iddhiyā pāramiṁ gatā;",
  "tha-ap3:174.3_0": "Paññāya pāramiṁ pattā,",
  "tha-ap3:174.4_0": "parivārenti taṁ sadā.",
  "tha-ap3:175.1_0": "Edisā te mahāvīra,",
  "tha-ap3:175.2_0": "tava sissā susikkhitā;",
  "tha-ap3:175.3_0": "Durāsadā uggatejā,",
  "tha-ap3:175.4_0": "parivārenti taṁ sadā.",
  "tha-ap3:176.1_0": "Tehi sissehi parivuto,",
  "tha-ap3:176.2_0": "saññatehi tapassibhi;",
  "tha-ap3:176.3_0": "Migarājāvasambhīto,",
  "tha-ap3:176.4_0": "uḷurājāva sobhasi.",
  "tha-ap3:177.1_0": "Pathaviyaṁ patiṭṭhāya,",
  "tha-ap3:177.2_0": "ruhanti dharaṇīruhā;",
  "tha-ap3:177.3_0": "Vepullataṁ pāpuṇanti,",
  "tha-ap3:177.4_0": "phalañca dassayanti te.",
  "tha-ap3:178.1_0": "Pathavīsadiso tvaṁsi,",
  "tha-ap3:178.2_0": "sakyaputta mahāyasa;",
  "tha-ap3:178.3_0": "Sāsane te patiṭṭhāya,",
  "tha-ap3:178.4_0": "labhanti amataṁ phalaṁ.",
  "tha-ap3:179.1_0": "Sindhu sarassatī ceva,",
  "tha-ap3:179.2_0": "nadiyo candabhāgikā;",
  "tha-ap3:179.3_0": "Gaṅgā ca yamunā ceva,",
  "tha-ap3:179.4_0": "sarabhū ca atho mahī.",
  "tha-ap3:180.1_0": "Etāsaṁ sandamānānaṁ,",
  "tha-ap3:180.2_0": "sāgaro sampaṭicchati;",
  "tha-ap3:180.3_0": "Jahanti purimaṁ nāmaṁ,",
  "tha-ap3:180.4_0": "sāgaroteva ñāyati.",
  "tha-ap3:181.1_0": "Tathevime catubbaṇṇā,",
  "tha-ap3:181.2_0": "pabbajitvā tavantike;",
  "tha-ap3:181.3_0": "Jahanti purimaṁ nāmaṁ,",
  "tha-ap3:181.4_0": "buddhaputtāti ñāyare.",
  "tha-ap3:182.1_0": "Yathāpi cando vimalo,",
  "tha-ap3:182.2_0": "gacchaṁ ākāsadhātuyā;",
  "tha-ap3:182.3_0": "Sabbe tāragaṇe loke,",
  "tha-ap3:182.4_0": "ābhāya atirocati.",
  "tha-ap3:183.1_0": "Tatheva tvaṁ mahāvīra,",
  "tha-ap3:183.2_0": "parivuto devamānuse;",
  "tha-ap3:183.3_0": "Ete sabbe atikkamma,",
  "tha-ap3:183.4_0": "jalasi sabbadā tuvaṁ.",
  "tha-ap3:184.1_0": "Gambhīre uṭṭhitā ūmī,",
  "tha-ap3:184.2_0": "na velamativattare;",
  "tha-ap3:184.3_0": "Sabbā velaṁva phusanti,",
  "tha-ap3:184.4_0": "sañcuṇṇā vikiranti tā.",
  "tha-ap3:185.1_0": "Tatheva titthiyā loke,",
  "tha-ap3:185.2_0": "nānādiṭṭhī bahujjanā;",
  "tha-ap3:185.3_0": "Dhammaṁ vāditukāmā te,",
  "tha-ap3:185.4_0": "nātivattanti taṁ muniṁ.",
  "tha-ap3:186.1_0": "Sace ca taṁ pāpuṇanti,",
  "tha-ap3:186.2_0": "paṭivādehi cakkhuma;",
  "tha-ap3:186.3_0": "Tavantikaṁ upāgantvā,",
  "tha-ap3:186.4_0": "sañcuṇṇāva bhavanti te.",
  "tha-ap3:187.1_0": "Yathāpi udake jātā,",
  "tha-ap3:187.2_0": "kumudā mandālakā bahū;",
  "tha-ap3:187.3_0": "Upalimpanti toyena,",
  "tha-ap3:187.4_0": "kaddamakalalena ca.",
  "tha-ap3:188.1_0": "Tatheva bahukā sattā,",
  "tha-ap3:188.2_0": "loke jātā virūhare;",
  "tha-ap3:188.3_0": "Aṭṭitā rāgadosena,",
  "tha-ap3:188.4_0": "kaddame kumudaṁ yathā.",
  "tha-ap3:189.1_0": "Yathāpi padumaṁ jalajaṁ,",
  "tha-ap3:189.2_0": "jalamajjhe virūhati;",
  "tha-ap3:189.3_0": "Na so limpati toyena,",
  "tha-ap3:189.4_0": "parisuddho hi kesarī.",
  "tha-ap3:190.1_0": "Tatheva tvaṁ mahāvīra,",
  "tha-ap3:190.2_0": "loke jāto mahāmuni;",
  "tha-ap3:190.3_0": "Nopalimpasi lokena,",
  "tha-ap3:190.4_0": "toyena padumaṁ yathā.",
  "tha-ap3:191.1_0": "Yathāpi rammake māse,",
  "tha-ap3:191.2_0": "bahū pupphanti vārijā;",
  "tha-ap3:191.3_0": "Nātikkamanti taṁ māsaṁ,",
  "tha-ap3:191.4_0": "samayo pupphanāya so.",
  "tha-ap3:192.1_0": "Tatheva tvaṁ mahāvīra,",
  "tha-ap3:192.2_0": "pupphito te vimuttiyā;",
  "tha-ap3:192.3_0": "Sāsanaṁ nātivattanti,",
  "tha-ap3:192.4_0": "padumaṁ vārijaṁ yathā.",
  "tha-ap3:193.1_0": "Supupphito sālarājā,",
  "tha-ap3:193.2_0": "dibbagandhaṁ pavāyati;",
  "tha-ap3:193.3_0": "Aññasālehi parivuto,",
  "tha-ap3:193.4_0": "sālarājāva sobhati.",
  "tha-ap3:194.1_0": "Tatheva tvaṁ mahāvīra,",
  "tha-ap3:194.2_0": "buddhañāṇena pupphito;",
  "tha-ap3:194.3_0": "Bhikkhusaṅghaparivuto,",
  "tha-ap3:194.4_0": "sālarājāva sobhasi.",
  "tha-ap3:195.1_0": "Yathāpi selo himavā,",
  "tha-ap3:195.2_0": "osadho sabbapāṇinaṁ;",
  "tha-ap3:195.3_0": "Nāgānaṁ asurānañca,",
  "tha-ap3:195.4_0": "devatānañca ālayo.",
  "tha-ap3:196.1_0": "Tatheva tvaṁ mahāvīra,",
  "tha-ap3:196.2_0": "osadho viya pāṇinaṁ;",
  "tha-ap3:196.3_0": "Tevijjā chaḷabhiññā ca,",
  "tha-ap3:196.4_0": "iddhiyā pāramiṁ gatā.",
  "tha-ap3:197.1_0": "Anusiṭṭhā mahāvīra,",
  "tha-ap3:197.2_0": "tayā kāruṇikena te;",
  "tha-ap3:197.3_0": "Ramanti dhammaratiyā,",
  "tha-ap3:197.4_0": "vasanti tava sāsane.",
  "tha-ap3:198.1_0": "Migarājā yathā sīho,",
  "tha-ap3:198.2_0": "abhinikkhamma āsayā;",
  "tha-ap3:198.3_0": "Catuddisānuviloketvā,",
  "tha-ap3:198.4_0": "tikkhattuṁ abhinādati.",
  "tha-ap3:199.1_0": "Sabbe migā uttasanti,",
  "tha-ap3:199.2_0": "migarājassa gajjato;",
  "tha-ap3:199.3_0": "Tathā hi jātimā eso,",
  "tha-ap3:199.4_0": "pasū tāseti sabbadā.",
  "tha-ap3:200.1_0": "Gajjato te mahāvīra,",
  "tha-ap3:200.2_0": "vasudhā sampakampati;",
  "tha-ap3:200.3_0": "Bodhaneyyāvabujjhanti,",
  "tha-ap3:200.4_0": "tasanti mārakāyikā.",
  "tha-ap3:201.1_0": "Tasanti titthiyā sabbe,",
  "tha-ap3:201.2_0": "nadato te mahāmuni;",
  "tha-ap3:201.3_0": "Kākā senāva vibbhantā,",
  "tha-ap3:201.4_0": "migaraññā yathā migā.",
  "tha-ap3:202.1_0": "Ye keci gaṇino loke,",
  "tha-ap3:202.2_0": "satthāroti pavuccare;",
  "tha-ap3:202.3_0": "Paramparāgataṁ dhammaṁ,",
  "tha-ap3:202.4_0": "desenti parisāya te.",
  "tha-ap3:203.1_0": "Na hevaṁ tvaṁ mahāvīra,",
  "tha-ap3:203.2_0": "dhammaṁ desesi pāṇinaṁ;",
  "tha-ap3:203.3_0": "Sāmaṁ saccāni bujjhitvā,",
  "tha-ap3:203.4_0": "kevalaṁ bodhipakkhiyaṁ.",
  "tha-ap3:204.1_0": "Āsayānusayaṁ ñatvā,",
  "tha-ap3:204.2_0": "indriyānaṁ balābalaṁ;",
  "tha-ap3:204.3_0": "Bhabbābhabbe viditvāna,",
  "tha-ap3:204.4_0": "mahāmeghova gajjasi.",
  "tha-ap3:205.1_0": "Cakkavāḷapariyantā,",
  "tha-ap3:205.2_0": "nisinnā parisā bhave;",
  "tha-ap3:205.3_0": "Nānādiṭṭhī vicinantā,",
  "tha-ap3:205.4_0": "vimaticchedanāya taṁ.",
  "tha-ap3:206.1_0": "Sabbesaṁ cittamaññāya,",
  "tha-ap3:206.2_0": "opammakusalo muni;",
  "tha-ap3:206.3_0": "Ekaṁ pañhaṁ kathentova,",
  "tha-ap3:206.4_0": "vimatiṁ chindasi pāṇinaṁ.",
  "tha-ap3:207.1_0": "Upatissasadiseheva,",
  "tha-ap3:207.2_0": "vasudhā pūritā bhave;",
  "tha-ap3:207.3_0": "Sabbeva te pañjalikā,",
  "tha-ap3:207.4_0": "kittayuṁ lokanāyakaṁ.",
  "tha-ap3:208.1_0": "Kappaṁ vā te kittayantā,",
  "tha-ap3:208.2_0": "nānāvaṇṇehi kittayuṁ;",
  "tha-ap3:208.3_0": "Parimetuṁ na sakkeyyuṁ,",
  "tha-ap3:208.4_0": "appameyyo tathāgato.",
  "tha-ap3:209.1_0": "Yathāsakena thāmena,",
  "tha-ap3:209.2_0": "kittito hi mayā jino;",
  "tha-ap3:209.3_0": "Kappakoṭīpi kittentā,",
  "tha-ap3:209.4_0": "evamevaṁ pakittayuṁ.",
  "tha-ap3:210.1_0": "Sace hi koci devo vā,",
  "tha-ap3:210.2_0": "manusso vā susikkhito;",
  "tha-ap3:210.3_0": "Pametuṁ parikappeyya,",
  "tha-ap3:210.4_0": "vighātaṁva labheyya so.",
  "tha-ap3:211.1_0": "Sāsane te patiṭṭhāya,",
  "tha-ap3:211.2_0": "sakyaputta mahāyasa;",
  "tha-ap3:211.3_0": "Paññāya pāramiṁ gantvā,",
  "tha-ap3:211.4_0": "viharāmi anāsavo.",
  "tha-ap3:212.1_0": "Titthiye sampamaddāmi,",
  "tha-ap3:212.2_0": "vattemi jinasāsanaṁ;",
  "tha-ap3:212.3_0": "Dhammasenāpati ajja,",
  "tha-ap3:212.4_0": "sakyaputtassa sāsane.",
  "tha-ap3:213.1_0": "Aparimeyye kataṁ kammaṁ,",
  "tha-ap3:213.2_0": "phalaṁ dassesi me idha;",
  "tha-ap3:213.3_0": "Sukhitto saravegova,",
  "tha-ap3:213.4_0": "kilese jhāpayī mama.",
  "tha-ap3:214.1_0": "Yo koci manujo bhāraṁ,",
  "tha-ap3:214.2_0": "dhāreyya matthake sadā;",
  "tha-ap3:214.3_0": "Bhārena dukkhito assa,",
  "tha-ap3:214.4_0": "bhārehi bharito tathā.",
  "tha-ap3:215.1_0": "Ḍayhamāno tīhaggīhi,",
  "tha-ap3:215.2_0": "bhavesu saṁsariṁ ahaṁ;",
  "tha-ap3:215.3_0": "Bharito bhavabhārena,",
  "tha-ap3:215.4_0": "giriṁ uccārito yathā.",
  "tha-ap3:216.1_0": "Oropito ca me bhāro,",
  "tha-ap3:216.2_0": "bhavā ugghāṭitā mayā;",
  "tha-ap3:216.3_0": "Karaṇīyaṁ kataṁ sabbaṁ,",
  "tha-ap3:216.4_0": "sakyaputtassa sāsane.",
  "tha-ap3:217.1_0": "Yāvatā buddhakhettamhi,",
  "tha-ap3:217.2_0": "ṭhapetvā sakyapuṅgavaṁ;",
  "tha-ap3:217.3_0": "Ahaṁ aggomhi paññāya,",
  "tha-ap3:217.4_0": "sadiso me na vijjati.",
  "tha-ap3:218.1_0": "Samādhimhi sukusalo,",
  "tha-ap3:218.2_0": "iddhiyā pāramiṁ gato;",
  "tha-ap3:218.3_0": "Icchamāno cahaṁ ajja,",
  "tha-ap3:218.4_0": "sahassaṁ abhinimmine.",
  "tha-ap3:219.1_0": "Anupubbavihārassa,",
  "tha-ap3:219.2_0": "vasībhūto mahāmuni;",
  "tha-ap3:219.3_0": "Kathesi sāsanaṁ mayhaṁ,",
  "tha-ap3:219.4_0": "nirodho sayanaṁ mama.",
  "tha-ap3:220.1_0": "Dibbacakkhu visuddhaṁ me,",
  "tha-ap3:220.2_0": "samādhikusalo ahaṁ;",
  "tha-ap3:220.3_0": "Sammappadhānānuyutto,",
  "tha-ap3:220.4_0": "bojjhaṅgabhāvanārato.",
  "tha-ap3:221.1_0": "Sāvakena hi pattabbaṁ,",
  "tha-ap3:221.2_0": "sabbameva kataṁ mayā;",
  "tha-ap3:221.3_0": "Lokanāthaṁ ṭhapetvāna,",
  "tha-ap3:221.4_0": "sadiso me na vijjati.",
  "tha-ap3:222.1_0": "Samāpattīnaṁ kusalo,",
  "tha-ap3:222.2_0": "Jhānavimokkhāna khippapaṭilābhī;",
  "tha-ap3:222.3_0": "Bojjhaṅgabhāvanārato,",
  "tha-ap3:222.4_0": "Sāvakaguṇapāramigatosmi.",
  "tha-ap3:223.1_0": "Sāvakaguṇenapi phussena,",
  "tha-ap3:223.2_0": "Buddhiyā parisuttamabhāravā;",
  "tha-ap3:223.3_0": "Yaṁ saddhāsaṅgahitaṁ cittaṁ,",
  "tha-ap3:223.4_0": "Sadā sabrahmacārīsu.",
  "tha-ap3:224.1_0": "Uddhatavisova sappo,",
  "tha-ap3:224.2_0": "Chinnavisāṇova usabho;",
  "tha-ap3:224.3_0": "Nikkhittamānadappova,",
  "tha-ap3:224.4_0": "Upemi garugāravena gaṇaṁ.",
  "tha-ap3:225.1_0": "Yadi rūpinī bhaveyya,",
  "tha-ap3:225.2_0": "Paññā me vasumatīpi na sameyya;",
  "tha-ap3:225.3_0": "Anomadassissa bhagavato,",
  "tha-ap3:225.4_0": "Phalametaṁ ñāṇathavanāya.",
  "tha-ap3:226.1_0": "Pavattitaṁ dhammacakkaṁ,",
  "tha-ap3:226.2_0": "sakyaputtena tādinā;",
  "tha-ap3:226.3_0": "Anuvattemahaṁ sammā,",
  "tha-ap3:226.4_0": "ñāṇathavanāyidaṁ phalaṁ.",
  "tha-ap3:227.1_0": "Mā me kadāci pāpiccho,",
  "tha-ap3:227.2_0": "kusīto hīnavīriyo;",
  "tha-ap3:227.3_0": "Appassuto anācāro,",
  "tha-ap3:227.4_0": "sameto ahu katthaci.",
  "tha-ap3:228.1_0": "Bahussuto ca medhāvī,",
  "tha-ap3:228.2_0": "sīlesu susamāhito;",
  "tha-ap3:228.3_0": "Cetosamathānuyutto,",
  "tha-ap3:228.4_0": "api muddhani tiṭṭhatu.",
  "tha-ap3:229.1_0": "Taṁ vo vadāmi bhaddante,",
  "tha-ap3:229.2_0": "yāvantettha samāgatā;",
  "tha-ap3:229.3_0": "Appicchā hotha santuṭṭhā,",
  "tha-ap3:229.4_0": "jhāyī jhānaratā sadā.",
  "tha-ap3:230.1_0": "Yamahaṁ paṭhamaṁ disvā,",
  "tha-ap3:230.2_0": "virajo vimalo ahuṁ;",
  "tha-ap3:230.3_0": "So me ācariyo dhīro,",
  "tha-ap3:230.4_0": "assaji nāma sāvako.",
  "tha-ap3:231.1_0": "Tassāhaṁ vāhasā ajja,",
  "tha-ap3:231.2_0": "dhammasenāpatī ahuṁ;",
  "tha-ap3:231.3_0": "Sabbattha pāramiṁ patvā,",
  "tha-ap3:231.4_0": "viharāmi anāsavo.",
  "tha-ap3:232.1_0": "Yo me ācariyo āsi,",
  "tha-ap3:232.2_0": "assaji nāma sāvako;",
  "tha-ap3:232.3_0": "Yassaṁ disāyaṁ vasati,",
  "tha-ap3:232.4_0": "ussīsamhi karomahaṁ.",
  "tha-ap3:233.1_0": "Mama kammaṁ saritvāna,",
  "tha-ap3:233.2_0": "gotamo sakyapuṅgavo;",
  "tha-ap3:233.3_0": "Bhikkhusaṅghe nisīditvā,",
  "tha-ap3:233.4_0": "aggaṭṭhāne ṭhapesi maṁ.",
  "tha-ap3:234.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap3:234.2_0": "bhavā sabbe samūhatā;",
  "tha-ap3:234.3_0": "Nāgova bandhanaṁ chetvā,",
  "tha-ap3:234.4_0": "viharāmi anāsavo.",
  "tha-ap3:235.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap3:235.2_0": "buddhaseṭṭhassa santike;",
  "tha-ap3:235.3_0": "Tisso vijjā anuppattā,",
  "tha-ap3:235.4_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap3:236.1_0": "Paṭisambhidā catasso,",
  "tha-ap3:236.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap3:236.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap3:236.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap3:237.1_0": "Itthaṁ sudaṁ āyasmā sāriputto thero imā gāthāyo abhāsitthāti.",
  "tha-ap3:238.1_0": "Sāriputtattherassāpadānaṁ paṭhamaṁ."
}