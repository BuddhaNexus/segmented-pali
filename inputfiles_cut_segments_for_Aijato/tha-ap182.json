{
  "tha-ap182:0.1_0": "Therāpadāna",
  "tha-ap182:0.2_0": "Kumudavagga",
  "tha-ap182:0.3_0": "10. Saṅghupaṭṭhākattheraapadāna",
  "tha-ap182:1.1_0": "“Vessabhumhi bhagavati,",
  "tha-ap182:1.2_0": "ahosārāmiko ahaṁ;",
  "tha-ap182:1.3_0": "Pasannacitto sumano,",
  "tha-ap182:1.4_0": "upaṭṭhiṁ saṅghamuttamaṁ.",
  "tha-ap182:2.1_0": "Ekattiṁse ito kappe,",
  "tha-ap182:2.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap182:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap182:2.4_0": "upaṭṭhānassidaṁ phalaṁ.",
  "tha-ap182:3.1_0": "Ito te sattame kappe,",
  "tha-ap182:3.2_0": "sattevāsuṁ samodakā;",
  "tha-ap182:3.3_0": "Sattaratanasampannā,",
  "tha-ap182:3.4_0": "cakkavattī mahabbalā.",
  "tha-ap182:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap182:4.2_0": "…pe…",
  "tha-ap182:4.3_0": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap182:5.1_0": "Itthaṁ sudaṁ āyasmā saṅghupaṭṭhāko thero imā gāthāyo abhāsitthāti.",
  "tha-ap182:6.1_0": "Saṅghupaṭṭhākattherassāpadānaṁ dasamaṁ.",
  "tha-ap182:7.1_0": "Kumudavaggo aṭṭhārasamo.",
  "tha-ap182:8.1_0": "Tassuddānaṁ",
  "tha-ap182:9.1_0": "Kumudo atha nisseṇī,",
  "tha-ap182:9.2_0": "rattiko udapānado;",
  "tha-ap182:9.3_0": "Sīhāsanī maggadado,",
  "tha-ap182:9.4_0": "ekadīpī maṇippado;",
  "tha-ap182:9.5_0": "Tikicchako upaṭṭhāko,",
  "tha-ap182:9.6_0": "ekapaññāsa gāthakāti."
}