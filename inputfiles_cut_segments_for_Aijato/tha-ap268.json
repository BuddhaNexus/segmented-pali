{
  "tha-ap268:0.1_0": "Therāpadāna",
  "tha-ap268:0.2_0": "Padumukkhipavagga",
  "tha-ap268:0.3_0": "6. Macchadāyakattheraapadāna",
  "tha-ap268:1.1_0": "“Candabhāgānadītīre,",
  "tha-ap268:1.2_0": "ukkuso āsahaṁ tadā;",
  "tha-ap268:1.3_0": "Mahantaṁ macchaṁ paggayha,",
  "tha-ap268:1.4_0": "siddhatthamunino adaṁ.",
  "tha-ap268:2.1_0": "Catunnavutito kappe,",
  "tha-ap268:2.2_0": "yaṁ macchamadadiṁ tadā;",
  "tha-ap268:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap268:2.4_0": "macchadānassidaṁ phalaṁ.",
  "tha-ap268:3.1_0": "Paṭisambhidā catasso,",
  "tha-ap268:3.2_0": "…pe…",
  "tha-ap268:3.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap268:4.1_0": "Itthaṁ sudaṁ āyasmā macchadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap268:5.1_0": "Macchadāyakattherassāpadānaṁ chaṭṭhaṁ."
}