{
  "tha-ap129:0.1_0": "Therāpadāna",
  "tha-ap129:0.2_0": "Sereyyavagga",
  "tha-ap129:0.3_0": "7. Phaladāyakattheraapadāna",
  "tha-ap129:1.1_0": "“Ajjhāyako mantadharo,",
  "tha-ap129:1.2_0": "tiṇṇaṁ vedāna pāragū;",
  "tha-ap129:1.3_0": "Himavantassāvidūre,",
  "tha-ap129:1.4_0": "vasāmi assame ahaṁ.",
  "tha-ap129:2.1_0": "Aggihuttañca me atthi,",
  "tha-ap129:2.2_0": "puṇḍarīkaphalāni ca;",
  "tha-ap129:2.3_0": "Puṭake nikkhipitvāna,",
  "tha-ap129:2.4_0": "dumagge laggitaṁ mayā.",
  "tha-ap129:3.1_0": "Padumuttaro lokavidū,",
  "tha-ap129:3.2_0": "āhutīnaṁ paṭiggaho;",
  "tha-ap129:3.3_0": "Mamuddharitukāmo so,",
  "tha-ap129:3.4_0": "bhikkhanto mamupāgami.",
  "tha-ap129:4.1_0": "Pasannacitto sumano,",
  "tha-ap129:4.2_0": "phalaṁ buddhassadāsahaṁ;",
  "tha-ap129:4.3_0": "Vittisañjanano mayhaṁ,",
  "tha-ap129:4.4_0": "diṭṭhadhammasukhāvaho.",
  "tha-ap129:5.1_0": "Suvaṇṇavaṇṇo sambuddho,",
  "tha-ap129:5.2_0": "āhutīnaṁ paṭiggaho;",
  "tha-ap129:5.3_0": "Antalikkhe ṭhito satthā,",
  "tha-ap129:5.4_0": "imaṁ gāthaṁ abhāsatha.",
  "tha-ap129:6.1_0": "‘Iminā phaladānena,",
  "tha-ap129:6.2_0": "cetanāpaṇidhīhi ca;",
  "tha-ap129:6.3_0": "Kappānaṁ satasahassaṁ,",
  "tha-ap129:6.4_0": "duggatiṁ nupapajjati’.",
  "tha-ap129:7.1_0": "Teneva sukkamūlena,",
  "tha-ap129:7.2_0": "anubhotvāna sampadā;",
  "tha-ap129:7.3_0": "Pattomhi acalaṁ ṭhānaṁ,",
  "tha-ap129:7.4_0": "hitvā jayaparājayaṁ.",
  "tha-ap129:8.1_0": "Ito sattasate kappe,",
  "tha-ap129:8.2_0": "rājā āsiṁ sumaṅgalo;",
  "tha-ap129:8.3_0": "Sattaratanasampanno,",
  "tha-ap129:8.4_0": "cakkavattī mahabbalo.",
  "tha-ap129:9.1_0": "Paṭisambhidā catasso,",
  "tha-ap129:9.2_0": "…pe…",
  "tha-ap129:9.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap129:10.1_0": "Itthaṁ sudaṁ āyasmā phaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap129:11.1_0": "Phaladāyakattherassāpadānaṁ sattamaṁ."
}