{
  "tha-ap86:0.1_0": "Therāpadāna",
  "tha-ap86:0.2_0": "Timiravagga",
  "tha-ap86:0.3_0": "4. Adhopupphiyattheraapadāna",
  "tha-ap86:1.1_0": "“Abhibhū nāma so bhikkhu,",
  "tha-ap86:1.2_0": "sikhino aggasāvako;",
  "tha-ap86:1.3_0": "Mahānubhāvo tevijjo,",
  "tha-ap86:1.4_0": "himavantaṁ upāgami.",
  "tha-ap86:2.1_0": "Ahampi himavantamhi,",
  "tha-ap86:2.2_0": "ramaṇīyassame isi;",
  "tha-ap86:2.3_0": "Vasāmi appamaññāsu,",
  "tha-ap86:2.4_0": "iddhīsu ca tadā vasī.",
  "tha-ap86:3.1_0": "Pakkhijāto viyākāse,",
  "tha-ap86:3.2_0": "pabbataṁ adhivatthayiṁ;",
  "tha-ap86:3.3_0": "Adhopupphaṁ gahetvāna,",
  "tha-ap86:3.4_0": "āgacchiṁ pabbataṁ ahaṁ.",
  "tha-ap86:4.1_0": "Satta pupphāni gaṇhitvā,",
  "tha-ap86:4.2_0": "matthake okiriṁ ahaṁ;",
  "tha-ap86:4.3_0": "Ālokite ca vīrena,",
  "tha-ap86:4.4_0": "pakkāmiṁ pācināmukho.",
  "tha-ap86:5.1_0": "Āvāsaṁ abhisambhosiṁ,",
  "tha-ap86:5.2_0": "patvāna assamaṁ ahaṁ;",
  "tha-ap86:5.3_0": "Khāribhāraṁ gahetvāna,",
  "tha-ap86:5.4_0": "pāyāsiṁ pabbatantaraṁ.",
  "tha-ap86:6.1_0": "Ajagaro maṁ pīḷesi,",
  "tha-ap86:6.2_0": "ghorarūpo mahabbalo;",
  "tha-ap86:6.3_0": "Pubbakammaṁ saritvāna,",
  "tha-ap86:6.4_0": "tattha kālaṅkato ahaṁ.",
  "tha-ap86:7.1_0": "Ekattiṁse ito kappe,",
  "tha-ap86:7.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap86:7.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap86:7.4_0": "pupphapūjāyidaṁ phalaṁ.",
  "tha-ap86:8.1_0": "Paṭisambhidā catasso,",
  "tha-ap86:8.2_0": "…pe…",
  "tha-ap86:8.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap86:9.1_0": "Itthaṁ sudaṁ āyasmā adhopupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap86:10.1_0": "Adhopupphiyattherassāpadānaṁ catutthaṁ."
}