{
  "tha-ap155:0.1_0": "Therāpadāna",
  "tha-ap155:0.2_0": "Bandhujīvakavagga",
  "tha-ap155:0.3_0": "3. Vīthisammajjakattheraapadāna",
  "tha-ap155:1.1_0": "“Udentaṁ sataraṁsiṁva,",
  "tha-ap155:1.2_0": "pītaraṁsiṁva bhāṇumaṁ;",
  "tha-ap155:1.3_0": "Pannarase yathā candaṁ,",
  "tha-ap155:1.4_0": "niyyantaṁ lokanāyakaṁ.",
  "tha-ap155:2.1_0": "Aṭṭhasaṭṭhisahassāni,",
  "tha-ap155:2.2_0": "sabbe khīṇāsavā ahuṁ;",
  "tha-ap155:2.3_0": "Parivāriṁsu sambuddhaṁ,",
  "tha-ap155:2.4_0": "dvipadindaṁ narāsabhaṁ.",
  "tha-ap155:3.1_0": "Sammajjitvāna taṁ vīthiṁ,",
  "tha-ap155:3.2_0": "niyyante lokanāyake;",
  "tha-ap155:3.3_0": "Ussāpesiṁ dhajaṁ tattha,",
  "tha-ap155:3.4_0": "vippasannena cetasā.",
  "tha-ap155:4.1_0": "Ekanavutito kappe,",
  "tha-ap155:4.2_0": "yaṁ dhajaṁ abhiropayiṁ;",
  "tha-ap155:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap155:4.4_0": "dhajadānassidaṁ phalaṁ.",
  "tha-ap155:5.1_0": "Ito catutthake kappe,",
  "tha-ap155:5.2_0": "rājāhosiṁ mahabbalo;",
  "tha-ap155:5.3_0": "Sabbākārena sampanno,",
  "tha-ap155:5.4_0": "sudhajo iti vissuto.",
  "tha-ap155:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap155:6.2_0": "…pe…",
  "tha-ap155:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap155:7.1_0": "Itthaṁ sudaṁ āyasmā vīthisammajjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap155:8.1_0": "Vīthisammajjakattherassāpadānaṁ tatiyaṁ."
}