{
  "anya-e1004n1:0_0": "Sambhedakathā",
  "anya-e1004n1:1_0": "81.",
  "anya-e1004n1:2_0": " Sattha kappavicārena,",
  "anya-e1004n1:3_0": "Kālo gacchati dhīmataṁ;",
  "anya-e1004n1:4_0": "Byasanena asādhūnaṁ,",
  "anya-e1004n1:5_0": "Niddāya kalahenavā.",
  "anya-e1004n1:6_0": "82.",
  "anya-e1004n1:7_0": " Sokaṭhānasahassāni,",
  "anya-e1004n1:8_0": "Bhayaṭhānasatānica;",
  "anya-e1004n1:9_0": "Divasedivase muḷhaṁ,",
  "anya-e1004n1:10_0": "Āvīsanti napaṇḍitaṁ.",
  "anya-e1004n1:11_0": "83.",
  "anya-e1004n1:12_0": " Atidīgho mahāmuḷho,",
  "anya-e1004n1:13_0": "Majjhimoca vicakkhaṇo;",
  "anya-e1004n1:14_0": "Vāsudevaṁ purakkhitvā,",
  "anya-e1004n1:15_0": "Sabbevāmanakā saṭṭhā.",
  "anya-e1004n1:16_0": "84.",
  "anya-e1004n1:17_0": " Na loke sobhate muḷho,",
  "anya-e1004n1:18_0": "Kevalattapasaṁsako;",
  "anya-e1004n1:19_0": "Api sampihite kūpe,",
  "anya-e1004n1:20_0": "Katavijjo pakāsate.",
  "anya-e1004n1:21_0": "85.",
  "anya-e1004n1:22_0": " Madantadamanaṁ satthaṁ,",
  "anya-e1004n1:23_0": "Khalānaṁ kurute madaṁ;",
  "anya-e1004n1:24_0": "Cakkhusaṅkhārakaṁ tejaṁ,",
  "anya-e1004n1:25_0": "Uluṅkāna mivandhakaṁ.",
  "anya-e1004n1:26_0": "86.",
  "anya-e1004n1:27_0": " Atyappamapi sādhūnaṁ,",
  "anya-e1004n1:28_0": "Silālekheva tiṭṭhate;",
  "anya-e1004n1:29_0": "Jalelekheva nīcānaṁ,",
  "anya-e1004n1:30_0": "Yaṁ kataṁ taṁpi nassati.",
  "anya-e1004n1:31_0": "87.",
  "anya-e1004n1:32_0": " Dabbamappaṁpi sādhūnaṁ,",
  "anya-e1004n1:33_0": "Jalaṁ kūpeva nissayo;",
  "anya-e1004n1:34_0": "Bahuttaṁpi asādhūnaṁ,",
  "anya-e1004n1:35_0": "Naca vāriva aṇṇave.",
  "anya-e1004n1:36_0": "88.",
  "anya-e1004n1:37_0": " Duṭṭhacitto panāhissa,",
  "anya-e1004n1:38_0": "Kodho pāsāṇalikkhito;",
  "anya-e1004n1:39_0": "Kujjhitabbe sujanassa,",
  "anya-e1004n1:40_0": "Jalelekhā ciraṭṭhito.",
  "anya-e1004n1:41_0": "89.",
  "anya-e1004n1:42_0": " Bālassa jīvitaṁ pāpaṁ,",
  "anya-e1004n1:43_0": "Mitarassi taraṁ bhave;",
  "anya-e1004n1:44_0": "Janakāyassa rājāva,",
  "anya-e1004n1:45_0": "Rājadhammova rājunaṁ.",
  "anya-e1004n1:46_0": "90.",
  "anya-e1004n1:47_0": " Sappādānaṁ balī sīho,",
  "anya-e1004n1:48_0": "Puḷāvako tato kippīlikā,",
  "anya-e1004n1:49_0": "Narā tato balī rājā,",
  "anya-e1004n1:50_0": "Sabbesa mantako balī.",
  "anya-e1004n1:51_0": "91.",
  "anya-e1004n1:52_0": " Bhūpoṇṇava ggi thī sippi,",
  "anya-e1004n1:53_0": "Abhijjhālucapuggalo;",
  "anya-e1004n1:54_0": "Etesaṁpi mahicchānaṁ,",
  "anya-e1004n1:55_0": "Mahicchitā aniccatā.",
  "anya-e1004n1:56_0": "92.",
  "anya-e1004n1:57_0": " Niddāluko asantuṭṭho,",
  "anya-e1004n1:58_0": "Akataññūca bhīruko;",
  "anya-e1004n1:59_0": "Sakkuṇantina sācāraṁ,",
  "anya-e1004n1:60_0": "Sikkhituṁ te kadācipi.",
  "anya-e1004n1:61_0": "93.",
  "anya-e1004n1:62_0": " Niddālu kāmarāmoca,",
  "anya-e1004n1:63_0": "Sukhito bhogavālaso;",
  "anya-e1004n1:64_0": "Nicchanto kammarāmo ca,",
  "anya-e1004n1:65_0": "Sattete satthavajjitā.",
  "anya-e1004n1:66_0": "94.",
  "anya-e1004n1:67_0": " Samiddho dhanadhaññena,",
  "anya-e1004n1:68_0": "Kaṭṭho dakatiṇaggihi;",
  "anya-e1004n1:69_0": "Sabbato duggatenaṭṭho,",
  "anya-e1004n1:70_0": "Tasmā nadukkataṁ kare.",
  "anya-e1004n1:71_0": "95.",
  "anya-e1004n1:72_0": " Puññapāpaphalaṁ yo ce,",
  "anya-e1004n1:73_0": "Nasaddhahati saccato;",
  "anya-e1004n1:74_0": "So ve khippaṁva attānaṁ,",
  "anya-e1004n1:75_0": "Ādāsatalamānaye.",
  "anya-e1004n1:76_0": "96.",
  "anya-e1004n1:77_0": " Samparāyikamatthaṁ yo,",
  "anya-e1004n1:78_0": "Nasaddhahati cepiso;",
  "anya-e1004n1:79_0": "Āvāse sagga gāmīnaṁ,",
  "anya-e1004n1:80_0": "Mākkabhe kiṁnapassati.",
  "anya-e1004n1:81_0": "97.",
  "anya-e1004n1:82_0": " Mahantaṁ vaṭṭarukkhādiṁ,",
  "anya-e1004n1:83_0": "Khuddabījaṁ bahūphalaṁ;",
  "anya-e1004n1:84_0": "Sakkhikatvā udikkheyya,",
  "anya-e1004n1:85_0": "Puññapāpakaro naro.",
  "anya-e1004n1:86_0": "98.",
  "anya-e1004n1:87_0": " Yassa sallahukaṁ hoti,",
  "anya-e1004n1:88_0": "Gurusakkāramānanaṁ;",
  "anya-e1004n1:89_0": "Tassa sallahukāyeva,",
  "anya-e1004n1:90_0": "Vijjāsampattisampadā.",
  "anya-e1004n1:91_0": "99.",
  "anya-e1004n1:92_0": " Upajjhācariyānañca,",
  "anya-e1004n1:93_0": "Mātāpitūnamevaca;",
  "anya-e1004n1:94_0": "Sakkaccaṁ yonupaṭṭhāti,",
  "anya-e1004n1:95_0": "Suto tassapitādiso.",
  "anya-e1004n1:96_0": "100.",
  "anya-e1004n1:97_0": " Dese dese kulānīca,",
  "anya-e1004n1:98_0": "Desedese ca bandhavo;",
  "anya-e1004n1:99_0": "Tādisaṁ sahajā yattha,",
  "anya-e1004n1:100_0": "Desaṁ passāminevatu.",
  "anya-e1004n1:101_0": "101.",
  "anya-e1004n1:102_0": " Puttaṁvā bhātaraṁ duṭṭhaṁ,",
  "anya-e1004n1:103_0": "Anusāseyya nojahe;",
  "anya-e1004n1:104_0": "Kiṁnu chejjaṁ karaṁpādaṁ,",
  "anya-e1004n1:105_0": "Littaṁ asucinā siyā;",
  "anya-e1004n1:106_0": "102.",
  "anya-e1004n1:107_0": " Bahuputte pitāeko,",
  "anya-e1004n1:108_0": "Avassaṁ poseti sadā;",
  "anya-e1004n1:109_0": "Bahuputtā nasakkonti,",
  "anya-e1004n1:110_0": "Posetuṁ pitarekakaṁ.",
  "anya-e1004n1:111_0": "103.",
  "anya-e1004n1:112_0": " Atijātamanujātaṁ ,",
  "anya-e1004n1:113_0": "Puttamicchanti paṇḍitā;",
  "anya-e1004n1:114_0": "Avajātaṁ naicchanti,",
  "anya-e1004n1:115_0": "Yohoti kulagandhano.",
  "anya-e1004n1:116_0": "104.",
  "anya-e1004n1:117_0": " Pañcaṭhānāni sampassaṁ,",
  "anya-e1004n1:118_0": "Puttamicchanti paṇḍitā;",
  "anya-e1004n1:119_0": "Jātovā no bharissati,",
  "anya-e1004n1:120_0": "Kiccaṁvā no karissati.",
  "anya-e1004n1:121_0": "105.",
  "anya-e1004n1:122_0": " Kulavaṁso ciraṁ tiṭṭhe,",
  "anya-e1004n1:123_0": "Dāyajjaṁ paṭipaccati;",
  "anya-e1004n1:124_0": "Athavāpana petānaṁ,",
  "anya-e1004n1:125_0": "Dakkhiṇaṁnupadassati.",
  "anya-e1004n1:126_0": "106.",
  "anya-e1004n1:127_0": " Antojāto dhanakkīto,",
  "anya-e1004n1:128_0": "Dāsabyopagato sayaṁ;",
  "anya-e1004n1:129_0": "Dāsākaramarānīto,",
  "anya-e1004n1:130_0": "Ccevante catudhā siyuṁ.",
  "anya-e1004n1:131_0": "107.",
  "anya-e1004n1:132_0": " Dāsā pañceva corayya-",
  "anya-e1004n1:133_0": "Sakhañātyattasadisā;",
  "anya-e1004n1:134_0": "Tathā viññūhi viññeyyā,",
  "anya-e1004n1:135_0": "Mittādārāca bandhavā.",
  "anya-e1004n1:136_0": "108.",
  "anya-e1004n1:137_0": " Tayova paṇḍitā satthe,",
  "anya-e1004n1:138_0": "Ahamevātivādīca,",
  "anya-e1004n1:139_0": "Ahamapiti vādīca,",
  "anya-e1004n1:140_0": "Nāhantica imetayo.",
  "anya-e1004n1:141_0": "109.",
  "anya-e1004n1:142_0": " Vinā satthaṁ nagaccheyya,",
  "anya-e1004n1:143_0": "Sūro saṅkāmabhūmiyaṁ;",
  "anya-e1004n1:144_0": "Paṇḍitaddhagū vāṇijjo,",
  "anya-e1004n1:145_0": "Videsagamano tathā.",
  "anya-e1004n1:146_0": "110.",
  "anya-e1004n1:147_0": " Sammā upaparikkhitvā,",
  "anya-e1004n1:148_0": "Akkharesu padesuca;",
  "anya-e1004n1:149_0": "Coraghāto siyā sisso,",
  "anya-e1004n1:150_0": "Guru coraṭṭakārako.",
  "anya-e1004n1:151_0": "111.",
  "anya-e1004n1:152_0": " Sādhuttaṁ sujanasamāgamā khalānaṁ,",
  "anya-e1004n1:153_0": "Sādhūnaṁ nakhalasamāgamā khalattaṁ;",
  "anya-e1004n1:154_0": "Āmodaṁkusumabhavaṁdadhātibhūmi,",
  "anya-e1004n1:155_0": "Bhūgandhaṁ naca kusumāni dhārayanti.",
  "anya-e1004n1:156_0": "112.",
  "anya-e1004n1:157_0": " Saṭṭhena mittaṁ kalusena dhammaṁ,",
  "anya-e1004n1:158_0": "Paropatāpena samiddhibhāvaṁ;",
  "anya-e1004n1:159_0": "Sukhena vijjaṁ pharusena nāriṁ,",
  "anya-e1004n1:160_0": "Icchanti ye te navapaṇḍitāva."
}