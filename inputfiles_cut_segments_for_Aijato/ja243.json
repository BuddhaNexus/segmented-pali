{
  "ja243:0.1_0": "Jātaka",
  "ja243:0.2_0": "Dukanipāta",
  "ja243:0.3_0": "Siṅgālavagga",
  "ja243:0.4_0": "3. Guttilajātaka",
  "ja243:1.1_0": "“Sattatantiṁ sumadhuraṁ,",
  "ja243:1.2_0": "rāmaṇeyyaṁ avācayiṁ;",
  "ja243:1.3_0": "So maṁ raṅgamhi avheti,",
  "ja243:1.4_0": "saraṇaṁ me hohi kosiya”.",
  "ja243:2.1_0": "“Ahaṁ taṁ saraṇaṁ samma,",
  "ja243:2.2_0": "Ahamācariyapūjako;",
  "ja243:2.3_0": "Na taṁ jayissati sisso,",
  "ja243:2.4_0": "Sissamācariya jessasī”ti.",
  "ja243:3.1_0": "Guttilajātakaṁ tatiyaṁ."
}