{
  "anya-e1101n0:0_0": "Namo tassa bhagavato arahato sammāsambuddhassa.",
  "anya-e1101n0:1_0": "Rasavāhinī",
  "anya-e1101n0:2_0": "Paṇāmādikathā",
  "anya-e1101n0:3_0": "1.",
  "anya-e1101n0:4_0": "Satthuppasatthacaraṇaṁ saraṇaṁ janānaṁ,",
  "anya-e1101n0:5_0": "Brahmādimoḷi maṇiraṁsi samāvahantaṁ,",
  "anya-e1101n0:6_0": "Paṅkeruhābhamudukomalacāruvaṇṇaṁ;",
  "anya-e1101n0:7_0": "Vandāmi cakkavaralakkhaṇamādadhānaṁ.",
  "anya-e1101n0:8_0": "2.",
  "anya-e1101n0:9_0": "Siddhaṁ jinena cirakālamatanditena,",
  "anya-e1101n0:10_0": "Yaṁ bhāvako samadhigacchati khemamaggaṁ;",
  "anya-e1101n0:11_0": "Yaṁ kapparukkha rucidāna maṇiva bhāti,",
  "anya-e1101n0:12_0": "Taṁ dhammamagga masamaṁ paṇamāmi niccaṁ.",
  "anya-e1101n0:13_0": "3.",
  "anya-e1101n0:14_0": "Santindriyaṁ sugatasūnuvaraṁ visuddhaṁ,",
  "anya-e1101n0:15_0": "Yaṁ dakkhiṇeyyamatadaṁ sucipuññakhettaṁ;",
  "anya-e1101n0:16_0": "Tāṇesinaṁ saraṇamujjhitasabbadukkhaṁ,",
  "anya-e1101n0:17_0": "Vandāmi saṅgha managhaṁ sirasā mahagghaṁ.",
  "anya-e1101n0:18_0": "4.",
  "anya-e1101n0:19_0": "Yampattamettha ratanattayathomanena,",
  "anya-e1101n0:20_0": "Puññena tena duritaṁ sakalaṁ paṇujja,",
  "anya-e1101n0:21_0": "Vakkhāmahaṁ sumadhuraṁ rasavāhinintaṁ,",
  "anya-e1101n0:22_0": "Bho bho suṇantu sujanā bhimudāvahā sā.",
  "anya-e1101n0:23_0": "5.",
  "anya-e1101n0:24_0": "Tatthatatthūpapannāni, vatthūni arahā pure;",
  "anya-e1101n0:25_0": "Abhāsuṁ dīpabhāsāya, ṭhapesuṁ taṁ purātanā.",
  "anya-e1101n0:26_0": "6.",
  "anya-e1101n0:27_0": "Mahāvihāre taṅgutta, vaṅkapariveṇavāsiko;",
  "anya-e1101n0:28_0": "Raṭṭhapāloti nāmena, sīlācāra guṇākaro.",
  "anya-e1101n0:29_0": "7.",
  "anya-e1101n0:30_0": "Hitāya parivattesi, pajānaṁ pāḷibhāsato;",
  "anya-e1101n0:31_0": "Punaruttādidosehi, tamāsi sabbamākulaṁ;",
  "anya-e1101n0:32_0": "Anākulaṁ karissāmi, taṁ suṇātha samāhitā.",
  "anya-e1101n0:33_0": "8.",
  "anya-e1101n0:34_0": "Vitarāgā pure vocuṁ, yasmā tasmā hi bhāsitaṁ;",
  "anya-e1101n0:35_0": "Etamādaraṇīyañhi, sādhu sādhūhi sabbadāti.",
  "anya-e1101n0:36_0": "Jambudīpuppatti vatthūni."
}