{
  "pli-tv-bu-vb-sk33:0.1_0": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk33:0.2_0": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk33:0.3_0": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk33:0.4_0": "Sakkaccavagga",
  "pli-tv-bu-vb-sk33:0.5_0": "33. Sapadānasikkhāpada",
  "pli-tv-bu-vb-sk33:1.1_0": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk33:1.2_0": "Tena kho pana samayena chabbaggiyā bhikkhū tahaṁ tahaṁ omasitvā piṇḍapātaṁ bhuñjanti …pe….",
  "pli-tv-bu-vb-sk33:1.3.1_0": "“Sapadānaṁ piṇḍapātaṁ bhuñjissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk33:1.4_0": "Sapadānaṁ piṇḍapāto bhuñjitabbo.",
  "pli-tv-bu-vb-sk33:1.5_0": "Yo anādariyaṁ paṭicca tahaṁ tahaṁ omasitvā piṇḍapātaṁ bhuñjati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk33:1.6.1_0": "Anāpatti—",
  "pli-tv-bu-vb-sk33:1.7_0": "asañcicca,",
  "pli-tv-bu-vb-sk33:1.8_0": "assatiyā,",
  "pli-tv-bu-vb-sk33:1.9_0": "ajānantassa,",
  "pli-tv-bu-vb-sk33:1.10_0": "gilānassa,",
  "pli-tv-bu-vb-sk33:1.11_0": "aññesaṁ dento omasati,",
  "pli-tv-bu-vb-sk33:1.12_0": "aññassa bhājane ākiranto omasati,",
  "pli-tv-bu-vb-sk33:1.13_0": "uttaribhaṅge,",
  "pli-tv-bu-vb-sk33:1.14_0": "āpadāsu,",
  "pli-tv-bu-vb-sk33:1.15_0": "ummattakassa,",
  "pli-tv-bu-vb-sk33:1.16_0": "ādikammikassāti.",
  "pli-tv-bu-vb-sk33:1.17_0": "Tatiyasikkhāpadaṁ niṭṭhitaṁ."
}