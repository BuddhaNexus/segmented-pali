{
  "thi-ap38:0.1_0": "Therīapadāna",
  "thi-ap38:0.2_0": "Khattiyāvagga",
  "thi-ap38:0.3_0": "8 Puṇṇikātherīapadāna",
  "thi-ap38:1.1_0": "“Vipassino bhagavato,",
  "thi-ap38:1.2_0": "sikhino vessabhussa ca;",
  "thi-ap38:1.3_0": "Kakusandhassa munino,",
  "thi-ap38:1.4_0": "koṇāgamanatādino.",
  "thi-ap38:2.1_0": "Kassapassa ca buddhassa,",
  "thi-ap38:2.2_0": "pabbajitvāna sāsane;",
  "thi-ap38:2.3_0": "Bhikkhunī sīlasampannā,",
  "thi-ap38:2.4_0": "nipakā saṁvutindriyā.",
  "thi-ap38:3.1_0": "Bahussutā dhammadharā,",
  "thi-ap38:3.2_0": "dhammatthapaṭipucchikā;",
  "thi-ap38:3.3_0": "Uggahetā ca dhammānaṁ,",
  "thi-ap38:3.4_0": "sotā payirupāsitā.",
  "thi-ap38:4.1_0": "Desentī janamajjhehaṁ,",
  "thi-ap38:4.2_0": "ahosiṁ jinasāsane;",
  "thi-ap38:4.3_0": "Bāhusaccena tenāhaṁ,",
  "thi-ap38:4.4_0": "pesalā atimaññisaṁ.",
  "thi-ap38:5.1_0": "Pacchime ca bhave dāni,",
  "thi-ap38:5.2_0": "sāvatthiyaṁ puruttame;",
  "thi-ap38:5.3_0": "Anāthapiṇḍino gehe,",
  "thi-ap38:5.4_0": "jātāhaṁ kumbhadāsiyā.",
  "thi-ap38:6.1_0": "Gatā udakahāriyaṁ,",
  "thi-ap38:6.2_0": "sotthiyaṁ dijamaddasaṁ;",
  "thi-ap38:6.3_0": "Sītaṭṭaṁ toyamajjhamhi,",
  "thi-ap38:6.4_0": "taṁ disvā idamabraviṁ.",
  "thi-ap38:7.1_0": "‘Udahārī ahaṁ sīte,",
  "thi-ap38:7.2_0": "sadā udakamotariṁ;",
  "thi-ap38:7.3_0": "Ayyānaṁ daṇḍabhayabhītā,",
  "thi-ap38:7.4_0": "vācādosabhayaṭṭitā.",
  "thi-ap38:8.1_0": "Kassa brāhmaṇa tvaṁ bhīto,",
  "thi-ap38:8.2_0": "sadā udakamotari;",
  "thi-ap38:8.3_0": "Vedhamānehi gattehi,",
  "thi-ap38:8.4_0": "sītaṁ vedayase bhusaṁ’.",
  "thi-ap38:9.1_0": "‘Jānantī vata maṁ bhoti,",
  "thi-ap38:9.2_0": "puṇṇike paripucchasi;",
  "thi-ap38:9.3_0": "Karontaṁ kusalaṁ kammaṁ,",
  "thi-ap38:9.4_0": "rundhantaṁ katapāpakaṁ.",
  "thi-ap38:10.1_0": "Yo ce vuḍḍho daharo vā,",
  "thi-ap38:10.2_0": "pāpakammaṁ pakubbati;",
  "thi-ap38:10.3_0": "Dakābhisiñcanā sopi,",
  "thi-ap38:10.4_0": "pāpakammā pamuccati’.",
  "thi-ap38:11.1_0": "Uttarantassa akkhāsiṁ,",
  "thi-ap38:11.2_0": "dhammatthasaṁhitaṁ padaṁ;",
  "thi-ap38:11.3_0": "Tañca sutvā sa saṁviggo,",
  "thi-ap38:11.4_0": "pabbajitvārahā ahu.",
  "thi-ap38:12.1_0": "Pūrentī ūnakasataṁ,",
  "thi-ap38:12.2_0": "jātā dāsikule yato;",
  "thi-ap38:12.3_0": "Tato puṇṇāti nāmaṁ me,",
  "thi-ap38:12.4_0": "bhujissaṁ maṁ akaṁsu te.",
  "thi-ap38:13.1_0": "Seṭṭhiṁ tatonujānetvā,",
  "thi-ap38:13.2_0": "pabbajiṁ anagāriyaṁ;",
  "thi-ap38:13.3_0": "Na cireneva kālena,",
  "thi-ap38:13.4_0": "arahattamapāpuṇiṁ.",
  "thi-ap38:14.1_0": "Iddhīsu ca vasī homi,",
  "thi-ap38:14.2_0": "dibbāya sotadhātuyā;",
  "thi-ap38:14.3_0": "Cetopariyañāṇassa,",
  "thi-ap38:14.4_0": "vasī homi mahāmune.",
  "thi-ap38:15.1_0": "Pubbenivāsaṁ jānāmi,",
  "thi-ap38:15.2_0": "dibbacakkhu visodhitaṁ;",
  "thi-ap38:15.3_0": "Sabbāsavā parikkhīṇā,",
  "thi-ap38:15.4_0": "natthi dāni punabbhavo.",
  "thi-ap38:16.1_0": "Atthadhammaniruttīsu,",
  "thi-ap38:16.2_0": "paṭibhāne tatheva ca;",
  "thi-ap38:16.3_0": "Ñāṇaṁ me vimalaṁ suddhaṁ,",
  "thi-ap38:16.4_0": "buddhaseṭṭhassa vāhasā.",
  "thi-ap38:17.1_0": "Bhāvanāya mahāpaññā,",
  "thi-ap38:17.2_0": "suteneva sutāvinī;",
  "thi-ap38:17.3_0": "Mānena nīcakulajā,",
  "thi-ap38:17.4_0": "na hi kammaṁ vinassati.",
  "thi-ap38:18.1_0": "Kilesā jhāpitā mayhaṁ,",
  "thi-ap38:18.2_0": "…pe…",
  "thi-ap38:18.3_0": "viharāmi anāsavā.",
  "thi-ap38:19.1_0": "Svāgataṁ vata me āsi,",
  "thi-ap38:19.2_0": "…pe…",
  "thi-ap38:19.3_0": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap38:20.1_0": "Paṭisambhidā catasso,",
  "thi-ap38:20.2_0": "…pe…",
  "thi-ap38:20.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "thi-ap38:21.1_0": "Itthaṁ sudaṁ puṇṇikā bhikkhunī imā gāthāyo abhāsitthāti.",
  "thi-ap38:22.1_0": "Puṇṇikātheriyāpadānaṁ aṭṭhamaṁ."
}