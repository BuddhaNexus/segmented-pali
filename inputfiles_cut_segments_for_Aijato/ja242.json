{
  "ja242:0.1_0": "Jātaka",
  "ja242:0.2_0": "Dukanipāta",
  "ja242:0.3_0": "Siṅgālavagga",
  "ja242:0.4_0": "2. Sunakhajātaka",
  "ja242:1.1_0": "“Bālo vatāyaṁ sunakho,",
  "ja242:1.2_0": "yo varattaṁ na khādati;",
  "ja242:1.3_0": "Bandhanā ca pamuñceyya,",
  "ja242:1.4_0": "asito ca gharaṁ vaje”.",
  "ja242:2.1_0": "“Aṭṭhitaṁ me manasmiṁ me,",
  "ja242:2.2_0": "atho me hadaye kataṁ;",
  "ja242:2.3_0": "Kālañca paṭikaṅkhāmi,",
  "ja242:2.4_0": "yāva passupatū jano”ti.",
  "ja242:3.1_0": "Sunakhajātakaṁ dutiyaṁ."
}