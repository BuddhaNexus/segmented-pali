{
  "tha-ap389:0.1_0": "Therāpadāna",
  "tha-ap389:0.2_0": "Avaṭaphalavagga",
  "tha-ap389:0.3_0": "7. Kadaliphaladāyakattheraapadāna",
  "tha-ap389:1.1_0": "“Kaṇikāraṁva jalitaṁ,",
  "tha-ap389:1.2_0": "puṇṇamāyeva candimaṁ;",
  "tha-ap389:1.3_0": "Jalantaṁ dīparukkhaṁva,",
  "tha-ap389:1.4_0": "addasaṁ lokanāyakaṁ.",
  "tha-ap389:2.1_0": "Kadaliphalaṁ paggayha,",
  "tha-ap389:2.2_0": "adāsiṁ satthuno ahaṁ;",
  "tha-ap389:2.3_0": "Pasannacitto sumano,",
  "tha-ap389:2.4_0": "vanditvāna apakkamiṁ.",
  "tha-ap389:3.1_0": "Ekattiṁse ito kappe,",
  "tha-ap389:3.2_0": "yaṁ phalamadadiṁ tadā;",
  "tha-ap389:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap389:3.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap389:4.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap389:4.2_0": "…pe…",
  "tha-ap389:4.3_0": "viharāmi anāsavo.",
  "tha-ap389:5.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap389:5.2_0": "…pe…",
  "tha-ap389:5.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap389:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap389:6.2_0": "…pe…",
  "tha-ap389:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap389:7.1_0": "Itthaṁ sudaṁ āyasmā kadaliphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap389:8.1_0": "Kadaliphaladāyakattherassāpadānaṁ sattamaṁ."
}