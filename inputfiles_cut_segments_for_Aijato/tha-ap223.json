{
  "tha-ap223:0.1_0": "Therāpadāna",
  "tha-ap223:0.2_0": "Ālambaṇadāyakavagga",
  "tha-ap223:0.3_0": "1 Ālambaṇadāyakattheraapadāna",
  "tha-ap223:1.1_0": "“Atthadassissa bhagavato,",
  "tha-ap223:1.2_0": "lokajeṭṭhassa tādino;",
  "tha-ap223:1.3_0": "Ālambaṇaṁ mayā dinnaṁ,",
  "tha-ap223:1.4_0": "dvipadindassa tādino.",
  "tha-ap223:2.1_0": "Dharaṇiṁ paṭipajjāmi,",
  "tha-ap223:2.2_0": "vipulaṁ sāgarapparaṁ;",
  "tha-ap223:2.3_0": "Pāṇesu ca issariyaṁ,",
  "tha-ap223:2.4_0": "vattemi vasudhāya ca.",
  "tha-ap223:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap223:3.2_0": "bhavā sabbe samūhatā;",
  "tha-ap223:3.3_0": "Tisso vijjā anuppattā,",
  "tha-ap223:3.4_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap223:4.1_0": "Ito dvesaṭṭhikappamhi,",
  "tha-ap223:4.2_0": "tayo āsiṁsu khattiyā;",
  "tha-ap223:4.3_0": "Ekāpassitanāmā te,",
  "tha-ap223:4.4_0": "cakkavattī mahabbalā.",
  "tha-ap223:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap223:5.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap223:5.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap223:5.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap223:6.1_0": "Itthaṁ sudaṁ āyasmā ālambaṇadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap223:7.1_0": "Ālambaṇadāyakattherassāpadānaṁ paṭhamaṁ."
}