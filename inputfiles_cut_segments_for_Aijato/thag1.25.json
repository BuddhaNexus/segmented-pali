{
  "thag1.25:0.1_0": "Theragāthā",
  "thag1.25:0.2_0": "Ekakanipāta",
  "thag1.25:0.3_0": "Tatiyavagga",
  "thag1.25:0.4_0": "1.25. Nandiyattheragāthā",
  "thag1.25:1.1_0": "“Obhāsajātaṁ phalagaṁ,",
  "thag1.25:1.2_0": "cittaṁ yassa abhiṇhaso;",
  "thag1.25:1.3_0": "Tādisaṁ bhikkhumāsajja,",
  "thag1.25:1.4_0": "kaṇha dukkhaṁ nigacchasī”ti.",
  "thag1.25:1.5_0": "… Nandiyo thero …."
}