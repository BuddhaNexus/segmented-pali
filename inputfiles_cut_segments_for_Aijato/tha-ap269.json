{
  "tha-ap269:0.1_0": "Therāpadāna",
  "tha-ap269:0.2_0": "Padumukkhipavagga",
  "tha-ap269:0.3_0": "7. Javahaṁsakattheraapadāna",
  "tha-ap269:1.1_0": "“Candabhāgānadītīre,",
  "tha-ap269:1.2_0": "Āsiṁ vanacaro tadā;",
  "tha-ap269:1.3_0": "Siddhatthaṁ addasaṁ buddhaṁ,",
  "tha-ap269:1.4_0": "Gacchantaṁ anilañjase.",
  "tha-ap269:2.1_0": "Añjaliṁ paggahetvāna,",
  "tha-ap269:2.2_0": "ullokento mahāmuniṁ;",
  "tha-ap269:2.3_0": "Sakaṁ cittaṁ pasādetvā,",
  "tha-ap269:2.4_0": "avandiṁ nāyakaṁ ahaṁ.",
  "tha-ap269:3.1_0": "Catunnavutito kappe,",
  "tha-ap269:3.2_0": "yamavandiṁ narāsabhaṁ;",
  "tha-ap269:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap269:3.4_0": "vandanāya idaṁ phalaṁ.",
  "tha-ap269:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap269:4.2_0": "…pe…",
  "tha-ap269:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap269:5.1_0": "Itthaṁ sudaṁ āyasmā javahaṁsako thero imā gāthāyo abhāsitthāti.",
  "tha-ap269:6.1_0": "Javahaṁsakattherassāpadānaṁ sattamaṁ."
}