{
  "anya-e1006n1:0_0": "Vasalasutta",
  "anya-e1006n1:1_0": "1.",
  "anya-e1006n1:2_0": " Kodhano upanāhīca,",
  "anya-e1006n1:3_0": "Pāpamakkhī ca yo naro;",
  "anya-e1006n1:4_0": "Vipannadiṭṭhī māyāvī,",
  "anya-e1006n1:5_0": "Taṁ jaññā vasalo iti.",
  "anya-e1006n1:6_0": "2.",
  "anya-e1006n1:7_0": " Ekajaṁ vā dvijaṁ vāpi,",
  "anya-e1006n1:8_0": "Yodha pāṇaṁ vihiṁsati;",
  "anya-e1006n1:9_0": "Yassa pāṇe dayā natthi,",
  "anya-e1006n1:10_0": "Taṁ jaññā vasalo iti.",
  "anya-e1006n1:11_0": "3.",
  "anya-e1006n1:12_0": " Yo hanti uparundhati,",
  "anya-e1006n1:13_0": "Gāmāni nigamāni ca;",
  "anya-e1006n1:14_0": "Niggāhako samaññāto,",
  "anya-e1006n1:15_0": "Taṁ jaññā vasalo iti.",
  "anya-e1006n1:16_0": "4.",
  "anya-e1006n1:17_0": " Gāme vā yadi vā raññe,",
  "anya-e1006n1:18_0": "Yaṁ paresaṁ mamāyitaṁ;",
  "anya-e1006n1:19_0": "Theyyā adinnaṁ ādeti,",
  "anya-e1006n1:20_0": "Taṁ jaññā vasalo iti.",
  "anya-e1006n1:21_0": "5.",
  "anya-e1006n1:22_0": " Yo have iṇamādāya,",
  "anya-e1006n1:23_0": "Vuccamāno palāyati;",
  "anya-e1006n1:24_0": "Na hi te iṇamatthīti,",
  "anya-e1006n1:25_0": "Taṁ jaññā vasalo iti.",
  "anya-e1006n1:26_0": "6.",
  "anya-e1006n1:27_0": " Yodha kiñcikkhakamyatā,",
  "anya-e1006n1:28_0": "Pathasmiṁ vajataṁ janaṁ;",
  "anya-e1006n1:29_0": "Hantvā kiñcikkha mādeti;",
  "anya-e1006n1:30_0": "Taṁ jaññā vasalo iti.",
  "anya-e1006n1:31_0": "7.",
  "anya-e1006n1:32_0": " Yo attahetu parahetu,",
  "anya-e1006n1:33_0": "Dhanahetu ca yo naro;",
  "anya-e1006n1:34_0": "Sakkhipuṭṭho musābrūti,",
  "anya-e1006n1:35_0": "Taṁ jaññā vasalo iti.",
  "anya-e1006n1:36_0": "8.",
  "anya-e1006n1:37_0": " Yo ñātīnaṁ sakhīnaṁ vā,",
  "anya-e1006n1:38_0": "Dāresu paṭidissati;",
  "anya-e1006n1:39_0": "Sahasā sampiyena vā,",
  "anya-e1006n1:40_0": "Taṁ jaññā vasalo iti.",
  "anya-e1006n1:41_0": "9.",
  "anya-e1006n1:42_0": " Yo 4 mātaraṁ pitaraṁ vā,",
  "anya-e1006n1:43_0": "Jiṇṇakaṁ gatayobbanaṁ;",
  "anya-e1006n1:44_0": "Pahusanto na bharati,",
  "anya-e1006n1:45_0": "Taṁ jaññā vasalo iti.",
  "anya-e1006n1:46_0": "10.",
  "anya-e1006n1:47_0": " Yo mātaraṁ pitaraṁ vā,",
  "anya-e1006n1:48_0": "Bhātaraṁ bhaginiṁ sassuṁ;",
  "anya-e1006n1:49_0": "Hanti roseti vācāya,",
  "anya-e1006n1:50_0": "Taṁ jaññā vasalo iti.",
  "anya-e1006n1:51_0": "11.",
  "anya-e1006n1:52_0": " Yo atthaṁ pucchito santo,",
  "anya-e1006n1:53_0": "Anattha manusāsati;",
  "anya-e1006n1:54_0": "Paṭicchannena manteti,",
  "anya-e1006n1:55_0": "Taṁ jaññā vasalo iti.",
  "anya-e1006n1:56_0": "12.",
  "anya-e1006n1:57_0": " Yo katvā pāpakaṁ kammaṁ,",
  "anya-e1006n1:58_0": "Māmaṁ jaññāti icchati;",
  "anya-e1006n1:59_0": "Yo paṭicchannakammanto,",
  "anya-e1006n1:60_0": "Taṁ jaññā vasalo iti.",
  "anya-e1006n1:61_0": "13.",
  "anya-e1006n1:62_0": " Yo ve parakulaṁ gantvā,",
  "anya-e1006n1:63_0": "Bhutvāna sucibhojanaṁ;",
  "anya-e1006n1:64_0": "Āgataṁ nappaṭipūjeti,",
  "anya-e1006n1:65_0": "Taṁ jaññā vasalo iti.",
  "anya-e1006n1:66_0": "14.",
  "anya-e1006n1:67_0": " Yo samaṇaṁ vā brāhmaṇaṁ,",
  "anya-e1006n1:68_0": "Aññaṁ vāpi vaṇibbakaṁ;",
  "anya-e1006n1:69_0": "Musāvādena vañceti,",
  "anya-e1006n1:70_0": "Taṁ jaññā vasalo iti.",
  "anya-e1006n1:71_0": "15.",
  "anya-e1006n1:72_0": " Yo samaṇaṁ vā brāhmaṇaṁ,",
  "anya-e1006n1:73_0": "Bhattakāle upaṭṭhitaṁ;",
  "anya-e1006n1:74_0": "Roseti vā na ca deti,",
  "anya-e1006n1:75_0": "Taṁ jaññā vasalo iti.",
  "anya-e1006n1:76_0": "16.",
  "anya-e1006n1:77_0": " Asantaṁ yodha pabrūti,",
  "anya-e1006n1:78_0": "Mohena paliguṇṭhito;",
  "anya-e1006n1:79_0": "Kiñcanaṁ nijigīsāno,",
  "anya-e1006n1:80_0": "Taṁ jaññā vasalo iti.",
  "anya-e1006n1:81_0": "17.",
  "anya-e1006n1:82_0": " Yocattānaṁ samukkaṁse,",
  "anya-e1006n1:83_0": "Pareca mavajānāti;",
  "anya-e1006n1:84_0": "Nihīno sena mānena,",
  "anya-e1006n1:85_0": "Taṁ jaññā vasalo iti.",
  "anya-e1006n1:86_0": "18.",
  "anya-e1006n1:87_0": " Rosako kadarīyo ca,",
  "anya-e1006n1:88_0": "Pāpiccho maccharī saṭho;",
  "anya-e1006n1:89_0": "Ahirīko anottappī,",
  "anya-e1006n1:90_0": "Taṁ jaññā vasalo iti.",
  "anya-e1006n1:91_0": "19.",
  "anya-e1006n1:92_0": " Yo buddhaṁ paribhāsati,",
  "anya-e1006n1:93_0": "Athavā tassa sāvakaṁ;",
  "anya-e1006n1:94_0": "Paribbajaṁ gahaṭṭhaṁ vā,",
  "anya-e1006n1:95_0": "Taṁ jaññā vasalo iti.",
  "anya-e1006n1:96_0": "Aṭṭhahi bhikkhave aṅgehi sampannāgatassa upāsakassa patto nikujjitabbo.",
  "anya-e1006n1:96_1": "Bhikkhūnaṁ alābhāya parisakkati, bhikkhūnaṁ anatthāya parisakkati, bhikkhūnaṁ anāvāsāya parisakkati, bhikkhūnaṁ akkosati paribhāsati, bhikkhūbhikkhūhi bhedeti, buddhassa avaṇṇaṁ bhāsati, dhammassa avaṇṇaṁ bhāsati, saṅghassa avaṇṇaṁ bhāsati, anujānāmi bhikkhave imehi aṭṭhahi aṅgehi sampannāgatassa upāsakassa pattaṁ nikujjituṁ.",
  "anya-e1006n1:97_0": "20.",
  "anya-e1006n1:98_0": " Yo anarahaṁ santo,",
  "anya-e1006n1:99_0": "Arahāti paṭijānāti;",
  "anya-e1006n1:100_0": "Coro sabrahmake loke,",
  "anya-e1006n1:101_0": "Taṁ jaññā vasalo iti.",
  "anya-e1006n1:102_0": "21.",
  "anya-e1006n1:103_0": " Ete kho vasalā vuttā,",
  "anya-e1006n1:104_0": "Mayā ye te pakāsitā;",
  "anya-e1006n1:105_0": "Na jaccā vasalo hoti,",
  "anya-e1006n1:106_0": "Na jaccā hoti brāhmaṇo.",
  "anya-e1006n1:107_0": "22.",
  "anya-e1006n1:108_0": " Kammunā vasalo hoti,",
  "anya-e1006n1:109_0": "Kammunā hoti brāhmaṇo.",
  "anya-e1006n1:110_0": "23.",
  "anya-e1006n1:111_0": " Tadamināpi jānātha,",
  "anya-e1006n1:112_0": "Yathāhetaṁ nidassanaṁ;",
  "anya-e1006n1:113_0": "Caṇḍālaputto sopāko,",
  "anya-e1006n1:114_0": "Mātaṅgo iti vissuto.",
  "anya-e1006n1:115_0": "24.",
  "anya-e1006n1:116_0": " So yasaṁ paramaṁ patto,",
  "anya-e1006n1:117_0": "Mātaṅgo yaṁ sudullabhaṁ;",
  "anya-e1006n1:118_0": "Āgacchuṁ tassupaṭṭhānaṁ,",
  "anya-e1006n1:119_0": "Khattiyā brāhmaṇā bahū.",
  "anya-e1006n1:120_0": "25.",
  "anya-e1006n1:121_0": " So devayānaṁ abhiruyha,",
  "anya-e1006n1:122_0": "Virajaṁ so mahāpathaṁ;",
  "anya-e1006n1:123_0": "Kāmarāgaṁ virājetvā,",
  "anya-e1006n1:124_0": "Brahmalokūpago ahu.",
  "anya-e1006n1:125_0": "Giriṁ nakhena khaṇasi,",
  "anya-e1006n1:126_0": "Ayo dantebhi khādasi;",
  "anya-e1006n1:127_0": "Jātavedaṁ padahasi,",
  "anya-e1006n1:128_0": "Yo isiṁ paribhāsati.",
  "anya-e1006n1:129_0": "Āvelitaṁ piṭṭhito uttamaṅgaṁ,",
  "anya-e1006n1:130_0": "Bāhuṁ pasāreti akampaṇeyyaṁ;",
  "anya-e1006n1:131_0": "Setāni akkhīni yathā matassa,",
  "anya-e1006n1:132_0": "Ko me imaṁ puttamakāsi evaṁ."
}