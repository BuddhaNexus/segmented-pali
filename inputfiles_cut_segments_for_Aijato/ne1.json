{
  "ne1:0.1_0": "Netti",
  "ne1:0.2_0": "1. Saṅgahavāra",
  "ne1:0.3_0": "Namo tassa Bhagavato Arahato Sammāsambuddhassa.",
  "ne1:1.1_0": "Yaṁ loko pūjayate,",
  "ne1:1.2_0": "Salokapālo sadā namassati ca;",
  "ne1:1.3_0": "Tasseta sāsanavaraṁ,",
  "ne1:1.4_0": "Vidūhi ñeyyaṁ naravarassa.",
  "ne1:2.1_0": "Dvādasa padāni suttaṁ,",
  "ne1:2.2_0": "Taṁ sabbaṁ byañjanañca attho ca;",
  "ne1:2.3_0": "Taṁ viññeyyaṁ ubhayaṁ,",
  "ne1:2.4_0": "Ko attho byañjanaṁ katamaṁ.",
  "ne1:3.1_0": "Soḷasahārā netti,",
  "ne1:3.2_0": "Pañcanayā sāsanassa pariyeṭṭhi;",
  "ne1:3.3_0": "Aṭṭhārasamūlapadā,",
  "ne1:3.4_0": "Mahakaccānena niddiṭṭhā.",
  "ne1:4.1_0": "Hārā byañjanavicayo,",
  "ne1:4.2_0": "Suttassa nayā tayo ca suttattho;",
  "ne1:4.3_0": "Ubhayaṁ pariggahītaṁ,",
  "ne1:4.4_0": "Vuccati suttaṁ yathāsuttaṁ.",
  "ne1:5.1_0": "Yā ceva desanā yañca,",
  "ne1:5.2_0": "Desitaṁ ubhayameva viññeyyaṁ;",
  "ne1:5.3_0": "Tatrāyamānupubbī,",
  "ne1:5.4_0": "Navavidhasuttantapariyeṭṭhīti.",
  "ne1:5.5_0": "Saṅgahavāro."
}