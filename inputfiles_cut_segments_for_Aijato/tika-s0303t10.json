{
  "tika-s0303t10:0_0": "11. Valāhakasaṁyuttavaṇṇanā",
  "tika-s0303t10:1_0": "550-606.",
  "tika-s0303t10:1_1": "Lokaṁ vālentā saṁvarantā chādentā ahanti pariyesantīti valāhā, devaputtā.",
  "tika-s0303t10:1_2": "Tesaṁ samūho valāhakadevakāyoti āha ‘‘valāhakakāyikā’’tiādi.",
  "tika-s0303t10:1_3": "Sītakaraṇavalāhakāti sītaharaṇavalāhakā.",
  "tika-s0303t10:1_4": "Sesapadesūti uṇhavalāhakādipadesu.",
  "tika-s0303t10:1_5": "Eseva nayoti ‘‘uṇhakaraṇavalāhakā’’tiādinā attho veditabbo.",
  "tika-s0303t10:1_6": "Cittaṭṭhapananti ‘‘sītaṁ hotū’’ti evaṁ cittassa uppādanaṁ. Vassāneti vassakāle.",
  "tika-s0303t10:1_7": "Utusamuṭṭhānamevāti pākatikasītamevāti attho. Uṇhepīti uṇhakāle.",
  "tika-s0303t10:1_8": "Abbhamaṇḍapoti maṇḍapasadisaabbhapaṭalavitānamāha.",
  "tika-s0303t10:1_9": "Abbhaṁ uppajjatīti tahaṁ tahaṁ paṭalaṁ uṭṭhahati.",
  "tika-s0303t10:1_10": "Abbheyevāti abbhakāle eva, vassāneti attho.",
  "tika-s0303t10:1_11": "Atiabbhanti satapaṭalasahassapaṭalaṁ hutvā abbhuṭṭhānaṁ.",
  "tika-s0303t10:1_12": "Cittavesākhamāsesūti vasantakālaṁ sandhāyāha.",
  "tika-s0303t10:1_13": "Tadā hi viddho vigatavalāhako devo bhavituṁ yutto.",
  "tika-s0303t10:1_14": "Uttaradakkhiṇādīti ādi-saddena pacchimavātādiṁ saṅgaṇhāti.",
  "tika-s0303t10:1_15": "Pakativātoti pakatiyā sabhāvena vāyanakavāto.",
  "tika-s0303t10:1_16": "Taṁ utusamuṭṭhānamevāti āhārūpajīvīnaṁ sattānaṁ sādhāraṇakammūpanissayautusamuṭṭhānameva.",
  "tika-s0303t10:1_17": "Esa nayo utusamuṭṭhānasītuṇhavātesupi.",
  "tika-s0303t10:1_18": "Tampi hi āhārūpajīvīnaṁ sattānaṁ sādhāraṇakammūpanissayamevāti.",
  "tika-s0303t10:2_0": "Gītanti meghagītaṁ.",
  "tika-s0303t10:2_1": "Saccakiriyāyāti tādisānaṁ purisavisesānaṁ saccādhiṭṭhānena.",
  "tika-s0303t10:2_2": "Iddhibalenāti iddhimantānaṁ iddhiānubhāvena.",
  "tika-s0303t10:2_3": "Vināsameghenāti kappavināsakameghena.",
  "tika-s0303t10:2_4": "Aññenapi kaṇhapāpikasattānaṁ pāpakammapaccayā uppannavināsameghena vuṭṭhena so so deso vinassateva.",
  "tika-s0303t10:3_0": "Valāhakasaṁyuttavaṇṇanā niṭṭhitā."
}