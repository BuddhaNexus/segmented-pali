{
  "anya-e0808n4:0_0": "5. Visamavuttiniddesa-pañcamapariccheda",
  "anya-e0808n4:1_0": "118.",
  "anya-e0808n4:1_1": "Na’ṭṭhakkharesu pādesu, snā’dimhā yo’ṇṇavā vattaṁ.",
  "anya-e0808n4:2_0": "119.",
  "anya-e0808n4:2_1": "Samesu sindhuto jena, pathyāvattaṁ pakittitaṁ.",
  "anya-e0808n4:3_0": "120. Ojesu jena sindhuto, ta’meva vīparītā’di.",
  "anya-e0808n4:4_0": "121. Na,kāro ce jaladhito, capalāvatta’micce’taṁ.",
  "anya-e0808n4:5_0": "122. Same lo sattamo yassā, vipulā piṅgalassa sā.",
  "anya-e0808n4:6_0": "123.Setavassā’khilesupi.",
  "anya-e0808n4:7_0": "124. Bhena’ṇṇavā bha’bbipulā.",
  "anya-e0808n4:8_0": "125. Eva’maññā ro catutthā.",
  "anya-e0808n4:9_0": "126. No’ṇṇavā ce navipulā.",
  "anya-e0808n4:10_0": "127. To’ṇṇavā tathā’ññā siyā.",
  "anya-e0808n4:11_0": "Vatta’ppabhedo.",
  "anya-e0808n4:12_0": "128.",
  "anya-e0808n4:13_0": " Nadissate’ttha yaṁ chandaṁ, payoge dissate yadi;",
  "anya-e0808n4:14_0": "Visama’kkharapādaṁ taṁ, gāthā sāmaññanāmato.",
  "anya-e0808n4:15_0": "Iti vuttodaye chandasi visamavutti niddeso nāma",
  "anya-e0808n4:16_0": "Pañcamo paricchedo."
}