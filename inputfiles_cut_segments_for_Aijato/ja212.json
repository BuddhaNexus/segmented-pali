{
  "ja212:0.1_0": "Jātaka",
  "ja212:0.2_0": "Dukanipāta",
  "ja212:0.3_0": "Bīraṇathambhavagga",
  "ja212:0.4_0": "2. Ucchiṭṭhabhattajātaka",
  "ja212:1.1_0": "“Añño uparimo vaṇṇo,",
  "ja212:1.2_0": "añño vaṇṇo ca heṭṭhimo;",
  "ja212:1.3_0": "Brāhmaṇī tveva pucchāmi,",
  "ja212:1.4_0": "kiṁ heṭṭhā kiñca uppari”.",
  "ja212:2.1_0": "“Ahaṁ naṭosmi bhaddante,",
  "ja212:2.2_0": "bhikkhakosmi idhāgato;",
  "ja212:2.3_0": "Ayañhi koṭṭhamotiṇṇo,",
  "ja212:2.4_0": "ayaṁ so yaṁ gavesasī”ti.",
  "ja212:3.1_0": "Ucchiṭṭhabhattajātakaṁ dutiyaṁ."
}