{
  "atk-s0402a50:0_0": "(15) 5. Ābhāvaggo",
  "atk-s0402a50:1_0": "1. Ābhāsuttavaṇṇanā",
  "atk-s0402a50:2_0": "141.",
  "atk-s0402a50:2_1": "Pañcamassa paṭhame ābhāsanavasena candova candābhā. Sesapadesupi eseva nayo.",
  "atk-s0402a50:3_0": "2-5. Pabhāsuttādivaṇṇanā",
  "atk-s0402a50:4_0": "142-145.",
  "atk-s0402a50:4_1": "Dutiyādīsupi pabhāsanavasena candova candappabhā.",
  "atk-s0402a50:4_2": "Ālokanavasena candova candāloko.",
  "atk-s0402a50:4_3": "Obhāsanavasena candova candobhāso.",
  "atk-s0402a50:4_4": "Pajjotanavasena candova candapajjototi.",
  "atk-s0402a50:4_5": "Evaṁ sabbapadesupi attho veditabbo.",
  "atk-s0402a50:5_0": "6. Paṭhamakālasuttavaṇṇanā",
  "atk-s0402a50:6_0": "146.",
  "atk-s0402a50:6_1": "Chaṭṭhe kālāti yuttappayuttakālā.",
  "atk-s0402a50:6_2": "Kālena dhammassavananti yuttappayuttakāle dhammassavanaṁ.",
  "atk-s0402a50:6_3": "Dhammasākacchāti pañhapucchanavissajjanavasena pavattā saṁsandanakathā.",
  "atk-s0402a50:7_0": "7. Dutiyakālasuttavaṇṇanā",
  "atk-s0402a50:8_0": "147.",
  "atk-s0402a50:8_1": "Sattame kālāti tasmiṁ tasmiṁ kāle dhammassavanādivasena pavattānaṁ kusaladhammānaṁ etaṁ adhivacanaṁ.",
  "atk-s0402a50:8_2": "Te bhāviyanti ceva anuparivattiyanti ca. Āsavānaṁkhayanti arahattaṁ. Aṭṭhamaṁ uttānatthameva.",
  "atk-s0402a50:9_0": "9-10. Sucaritasuttādivaṇṇanā",
  "atk-s0402a50:10_0": "149-150.",
  "atk-s0402a50:10_1": "Navame saṇhā vācāti mudukavācā.",
  "atk-s0402a50:10_2": "Mantabhāsāti mantasaṅkhātāya paññāya paricchinditvā kathitakathā.",
  "atk-s0402a50:10_3": "Dasame sīlasāroti sārasampāpakaṁ sīlaṁ. Sesesupi eseva nayo.",
  "atk-s0402a50:11_0": "Ābhāvaggo pañcamo.",
  "atk-s0402a50:12_0": "Tatiyapaṇṇāsakaṁ niṭṭhitaṁ.",
  "atk-s0402a50:13_0": "4. Catutthapaṇṇāsakaṁ"
}