{
  "kp3:0.1_0": "Khuddakapāṭha",
  "kp3:0.2_0": "3. Dvattiṁsākāra",
  "kp3:1.1_0": "Atthi imasmiṁ kāye—",
  "kp3:2.1_0": "Kesā lomā nakhā dantā taco,",
  "kp3:2.2_0": "maṁsaṁ nhāru aṭṭhi aṭṭhimiñjaṁ vakkaṁ,",
  "kp3:2.3_0": "hadayaṁ yakanaṁ kilomakaṁ pihakaṁ papphāsaṁ,",
  "kp3:2.4_0": "antaṁ antaguṇaṁ udariyaṁ karīsaṁ matthaluṅgaṁ,",
  "kp3:2.5_0": "pittaṁ semhaṁ pubbo lohitaṁ sedo medo,",
  "kp3:2.6_0": "assu vasā kheḷo siṅghāṇikā lasikā muttanti.",
  "kp3:3.1_0": "Dvattiṁsākāraṁ."
}