{
  "vv49:0.1_0": "Vimānavatthu",
  "vv49:0.2_0": "Itthivimāna",
  "vv49:0.3_0": "Mañjiṭṭhakavagga",
  "vv49:0.4_0": "11. Vandanavimānavatthu",
  "vv49:1.1_0": "“Abhikkantena vaṇṇena,",
  "vv49:1.2_0": "yā tvaṁ tiṭṭhasi devate;",
  "vv49:1.3_0": "Obhāsentī disā sabbā,",
  "vv49:1.4_0": "osadhī viya tārakā.",
  "vv49:2.1_0": "Kena tetādiso vaṇṇo,",
  "vv49:2.2_0": "…pe…",
  "vv49:2.3_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv49:3.1_0": "Sā devatā attamanā,",
  "vv49:3.2_0": "…pe…",
  "vv49:3.3_0": "yassa kammassidaṁ phalaṁ.",
  "vv49:4.1_0": "“Ahaṁ manussesu manussabhūtā,",
  "vv49:4.2_0": "Disvāna samaṇe sīlavante;",
  "vv49:4.3_0": "Pādāni vanditvā manaṁ pasādayiṁ,",
  "vv49:4.4_0": "Vittā cahaṁ añjalikaṁ akāsiṁ.",
  "vv49:5.1_0": "Tena metādiso vaṇṇo,",
  "vv49:5.2_0": "…pe…",
  "vv49:5.3_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv49:6.1_0": "Vandanavimānaṁ ekādasamaṁ."
}