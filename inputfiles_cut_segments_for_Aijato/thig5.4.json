{
  "thig5.4:0.1_0": "Therīgāthā",
  "thig5.4:0.2_0": "Pañcakanipāta",
  "thig5.4:0.3_0": "4. Sundarīnandātherīgāthā",
  "thig5.4:1.1_0": "“Āturaṁ asuciṁ pūtiṁ,",
  "thig5.4:1.2_0": "passa nande samussayaṁ;",
  "thig5.4:1.3_0": "Asubhāya cittaṁ bhāvehi,",
  "thig5.4:1.4_0": "ekaggaṁ susamāhitaṁ.",
  "thig5.4:2.1_0": "Yathā idaṁ tathā etaṁ,",
  "thig5.4:2.2_0": "yathā etaṁ tathā idaṁ;",
  "thig5.4:2.3_0": "Duggandhaṁ pūtikaṁ vāti,",
  "thig5.4:2.4_0": "bālānaṁ abhinanditaṁ.",
  "thig5.4:3.1_0": "Evametaṁ avekkhantī,",
  "thig5.4:3.2_0": "rattindivamatanditā;",
  "thig5.4:3.3_0": "Tato sakāya paññāya,",
  "thig5.4:3.4_0": "abhinibbijjha dakkhisaṁ”.",
  "thig5.4:4.1_0": "“Tassā me appamattāya,",
  "thig5.4:4.2_0": "vicinantiyā yoniso;",
  "thig5.4:4.3_0": "Yathābhūtaṁ ayaṁ kāyo,",
  "thig5.4:4.4_0": "diṭṭho santarabāhiro.",
  "thig5.4:5.1_0": "Atha nibbindahaṁ kāye,",
  "thig5.4:5.2_0": "ajjhattañca virajjahaṁ;",
  "thig5.4:5.3_0": "Appamattā visaṁyuttā,",
  "thig5.4:5.4_0": "upasantāmhi nibbutā”ti.",
  "thig5.4:6.1_0": "… Sundarīnandā therī …."
}