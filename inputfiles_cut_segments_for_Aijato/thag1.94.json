{
  "thag1.94:0.1_0": "Theragāthā",
  "thag1.94:0.2_0": "Ekakanipāta",
  "thag1.94:0.3_0": "Dasamavagga",
  "thag1.94:0.4_0": "1.94. Mettajittheragāthā",
  "thag1.94:1.1_0": "“Namo hi tassa bhagavato,",
  "thag1.94:1.2_0": "sakyaputtassa sirīmato;",
  "thag1.94:1.3_0": "Tenāyaṁ aggappattena,",
  "thag1.94:1.4_0": "aggadhammo sudesito”ti.",
  "thag1.94:1.5_0": "… Mettaji thero …."
}