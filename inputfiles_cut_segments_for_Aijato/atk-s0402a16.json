{
  "atk-s0402a16:0_0": "2. Akusalapeyyālaṁ",
  "atk-s0402a16:1_0": "191-200.Sāvajjāti sadosā.",
  "atk-s0402a16:1_1": "Anavajjāti niddosā.",
  "atk-s0402a16:1_2": "Dukkhudrayāti dukkhavaḍḍhikā.",
  "atk-s0402a16:1_3": "Sukhudriyāti sukhavaḍḍhikā.",
  "atk-s0402a16:1_4": "Sabyābajjhāti sadukkhā.",
  "atk-s0402a16:1_5": "Abyābajjhāti niddukkhā.",
  "atk-s0402a16:1_6": "Ettāvatā vaṭṭavivaṭṭameva kathitaṁ.",
  "atk-s0402a16:2_0": "Akusalapeyyālaṁ niṭṭhitaṁ."
}