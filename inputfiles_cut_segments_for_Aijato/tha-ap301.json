{
  "tha-ap301:0.1_0": "Therāpadāna",
  "tha-ap301:0.2_0": "Citakapūjakavagga",
  "tha-ap301:0.3_0": "9. Ambapiṇḍiyattheraapadāna",
  "tha-ap301:1.1_0": "“Romaso nāma nāmena,",
  "tha-ap301:1.2_0": "dānavo iti vissuto;",
  "tha-ap301:1.3_0": "Ambapiṇḍī mayā dinnā,",
  "tha-ap301:1.4_0": "vipassissa mahesino.",
  "tha-ap301:2.1_0": "Ekanavutito kappe,",
  "tha-ap301:2.2_0": "yamambamadadiṁ tadā;",
  "tha-ap301:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap301:2.4_0": "ambadānassidaṁ phalaṁ.",
  "tha-ap301:3.1_0": "Paṭisambhidā catasso,",
  "tha-ap301:3.2_0": "…pe…",
  "tha-ap301:3.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap301:4.1_0": "Itthaṁ sudaṁ āyasmā ambapiṇḍiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap301:5.1_0": "Ambapiṇḍiyattherassāpadānaṁ navamaṁ."
}