{
  "snp4.2:0.1_0": "Sutta Nipāta 4.2",
  "snp4.2:0.2_0": "Guhaṭṭhakasutta",
  "snp4.2:1.1_0": "Satto guhāyaṁ bahunābhichanno,",
  "snp4.2:1.2_0": "Tiṭṭhaṁ naro mohanasmiṁ pagāḷho;",
  "snp4.2:1.3_0": "Dūre vivekā hi tathāvidho so,",
  "snp4.2:1.4_0": "Kāmā hi loke na hi suppahāyā.",
  "snp4.2:2.1_0": "Icchānidānā bhavasātabaddhā,",
  "snp4.2:2.2_0": "Te duppamuñcā na hi aññamokkhā;",
  "snp4.2:2.3_0": "Pacchā pure vāpi apekkhamānā,",
  "snp4.2:2.4_0": "Ime va kāme purime va jappaṁ.",
  "snp4.2:3.1_0": "Kāmesu giddhā pasutā pamūḷhā,",
  "snp4.2:3.2_0": "Avadāniyā te visame niviṭṭhā;",
  "snp4.2:3.3_0": "Dukkhūpanītā paridevayanti,",
  "snp4.2:3.4_0": "Kiṁsū bhavissāma ito cutāse.",
  "snp4.2:4.1_0": "Tasmā hi sikkhetha idheva jantu,",
  "snp4.2:4.2_0": "Yaṁ kiñci jaññā visamanti loke;",
  "snp4.2:4.3_0": "Na tassa hetū visamaṁ careyya,",
  "snp4.2:4.4_0": "Appañhidaṁ jīvitamāhu dhīrā.",
  "snp4.2:5.1_0": "Passāmi loke pariphandamānaṁ,",
  "snp4.2:5.2_0": "Pajaṁ imaṁ taṇhagataṁ bhavesu;",
  "snp4.2:5.3_0": "Hīnā narā maccumukhe lapanti,",
  "snp4.2:5.4_0": "Avītataṇhāse bhavābhavesu.",
  "snp4.2:6.1_0": "Mamāyite passatha phandamāne,",
  "snp4.2:6.2_0": "Maccheva appodake khīṇasote;",
  "snp4.2:6.3_0": "Etampi disvā amamo careyya,",
  "snp4.2:6.4_0": "Bhavesu āsattimakubbamāno.",
  "snp4.2:7.1_0": "Ubhosu antesu vineyya chandaṁ,",
  "snp4.2:7.2_0": "Phassaṁ pariññāya anānugiddho;",
  "snp4.2:7.3_0": "Yadattagarahī tadakubbamāno,",
  "snp4.2:7.4_0": "Na lippatī diṭṭhasutesu dhīro.",
  "snp4.2:8.1_0": "Saññaṁ pariññā vitareyya oghaṁ,",
  "snp4.2:8.2_0": "Pariggahesu muni nopalitto;",
  "snp4.2:8.3_0": "Abbūḷhasallo caramappamatto,",
  "snp4.2:8.4_0": "Nāsīsatī lokamimaṁ parañcāti.",
  "snp4.2:8.5_0": "Guhaṭṭhakasuttaṁ dutiyaṁ."
}