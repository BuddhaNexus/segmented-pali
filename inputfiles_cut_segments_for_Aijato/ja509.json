{
  "ja509:0.1_0": "Jātaka",
  "ja509:0.2_0": "Vīsatinipāta",
  "ja509:0.3_0": "Mātaṅgavagga",
  "ja509:0.4_0": "13. Hatthipālajātaka",
  "ja509:1.1_0": "“Cirassaṁ vata passāma,",
  "ja509:1.2_0": "brāhmaṇaṁ devavaṇṇinaṁ;",
  "ja509:1.3_0": "Mahājaṭaṁ khāridharaṁ,",
  "ja509:1.4_0": "paṅkadantaṁ rajassiraṁ.",
  "ja509:2.1_0": "Cirassaṁ vata passāma,",
  "ja509:2.2_0": "isiṁ dhammaguṇe rataṁ;",
  "ja509:2.3_0": "Kāsāyavatthavasanaṁ,",
  "ja509:2.4_0": "vākacīraṁ paṭicchadaṁ.",
  "ja509:3.1_0": "Āsanaṁ udakaṁ pajjaṁ,",
  "ja509:3.2_0": "paṭigaṇhātu no bhavaṁ;",
  "ja509:3.3_0": "Agghe bhavantaṁ pucchāma,",
  "ja509:3.4_0": "agghaṁ kurutu no bhavaṁ”.",
  "ja509:4.1_0": "“Adhicca vede pariyesa vittaṁ,",
  "ja509:4.2_0": "Putte gahe tāta patiṭṭhapetvā;",
  "ja509:4.3_0": "Gandhe rase paccanubhuyya sabbaṁ,",
  "ja509:4.4_0": "Araññaṁ sādhu muni so pasattho”.",
  "ja509:5.1_0": "“Vedā na saccā na ca vittalābho,",
  "ja509:5.2_0": "Na puttalābhena jaraṁ vihanti;",
  "ja509:5.3_0": "Gandhe rase muccanamāhu santo,",
  "ja509:5.4_0": "Sakammunā hoti phalūpapatti”.",
  "ja509:6.1_0": "“Addhā hi saccaṁ vacanaṁ tavetaṁ,",
  "ja509:6.2_0": "Sakammunā hoti phalūpapatti;",
  "ja509:6.3_0": "Jiṇṇā ca mātāpitaro tavīme,",
  "ja509:6.4_0": "Passeyyuṁ taṁ vassasataṁ arogaṁ”.",
  "ja509:7.1_0": "“Yassassa sakkhī maraṇena rāja,",
  "ja509:7.2_0": "Jarāya mettī naravīraseṭṭha;",
  "ja509:7.3_0": "Yo cāpi jaññā na marissaṁ kadāci,",
  "ja509:7.4_0": "Passeyyuṁ taṁ vassasataṁ arogaṁ.",
  "ja509:8.1_0": "Yathāpi nāvaṁ puriso dakamhi,",
  "ja509:8.2_0": "Ereti ce naṁ upaneti tīraṁ;",
  "ja509:8.3_0": "Evampi byādhī satataṁ jarā ca,",
  "ja509:8.4_0": "Upaneti maccaṁ vasamantakassa”.",
  "ja509:9.1_0": "“Paṅko ca kāmā palipo ca kāmā,",
  "ja509:9.2_0": "Manoharā duttarā maccudheyyā;",
  "ja509:9.3_0": "Etasmiṁ paṅke palipe byasannā,",
  "ja509:9.4_0": "Hīnattarūpā na taranti pāraṁ.",
  "ja509:10.1_0": "Ayaṁ pure luddamakāsi kammaṁ,",
  "ja509:10.2_0": "Svāyaṁ gahīto na hi mokkhito me;",
  "ja509:10.3_0": "Orundhiyā naṁ parirakkhissāmi,",
  "ja509:10.4_0": "Māyaṁ puna luddamakāsi kammaṁ”.",
  "ja509:11.1_0": "“Gavaṁva naṭṭhaṁ puriso yathā vane,",
  "ja509:11.2_0": "Anvesatī rāja apassamāno;",
  "ja509:11.3_0": "Evaṁ naṭṭho esukārī mamattho,",
  "ja509:11.4_0": "Sohaṁ kathaṁ na gaveseyyaṁ rāja.",
  "ja509:12.1_0": "Hiyyoti hiyyati poso,",
  "ja509:12.2_0": "Pareti parihāyati;",
  "ja509:12.3_0": "Anāgataṁ netamatthīti ñatvā,",
  "ja509:12.4_0": "Uppannachandaṁ ko panudeyya dhīro”.",
  "ja509:13.1_0": "“Passāmi vohaṁ daharaṁ kumāriṁ,",
  "ja509:13.2_0": "Mattūpamaṁ ketakapupphanettaṁ;",
  "ja509:13.3_0": "Abhuttabhoge paṭhame vayasmiṁ,",
  "ja509:13.4_0": "Ādāya maccu vajate kumāriṁ.",
  "ja509:14.1_0": "Yuvā sujāto sumukho sudassano,",
  "ja509:14.2_0": "Sāmo kusumbhaparikiṇṇamassu;",
  "ja509:14.3_0": "Hitvāna kāme paṭikacca gehaṁ,",
  "ja509:14.4_0": "Anujāna maṁ pabbajissāmi deva”.",
  "ja509:15.1_0": "“Sākhāhi rukkho labhate samaññaṁ,",
  "ja509:15.2_0": "Pahīnasākhaṁ pana khāṇumāhu;",
  "ja509:15.3_0": "Pahīnaputtassa mamajja bhoti,",
  "ja509:15.4_0": "Vāseṭṭhi bhikkhācariyāya kālo”.",
  "ja509:16.1_0": "“Aghasmi koñcāva yathā himaccaye,",
  "ja509:16.2_0": "Katāni jālāni padāliya haṁsā;",
  "ja509:16.3_0": "Gacchanti puttā ca patī ca mayhaṁ,",
  "ja509:16.4_0": "Sāhaṁ kathaṁ nānuvaje pajānaṁ”.",
  "ja509:17.1_0": "“Ete bhutvā vamitvā ca,",
  "ja509:17.2_0": "pakkamanti vihaṅgamā;",
  "ja509:17.3_0": "Ye ca bhutvāna vamiṁsu,",
  "ja509:17.4_0": "te me hatthattamāgatā.",
  "ja509:18.1_0": "Avamī brāhmaṇo kāme,",
  "ja509:18.2_0": "so tvaṁ paccāvamissasi;",
  "ja509:18.3_0": "Vantādo puriso rāja,",
  "ja509:18.4_0": "na so hoti pasaṁsiyo”.",
  "ja509:19.1_0": "“Paṅke ca posaṁ palipe byasannaṁ,",
  "ja509:19.2_0": "Balī yathā dubbalamuddhareyya;",
  "ja509:19.3_0": "Evampi maṁ tvaṁ udatāri bhoti,",
  "ja509:19.4_0": "Pañcāli gāthāhi subhāsitāhi”.",
  "ja509:20.1_0": "Idaṁ vatvā mahārājā,",
  "ja509:20.2_0": "esukārī disampati;",
  "ja509:20.3_0": "Raṭṭhaṁ hitvāna pabbaji,",
  "ja509:20.4_0": "nāgo chetvāva bandhanaṁ.",
  "ja509:21.1_0": "“Rājā ca pabbajjamarocayittha,",
  "ja509:21.2_0": "Raṭṭhaṁ pahāya naravīraseṭṭho;",
  "ja509:21.3_0": "Tuvampi no hohi yatheva rājā,",
  "ja509:21.4_0": "Amhehi guttā anusāsa rajjaṁ”.",
  "ja509:22.1_0": "“Rājā ca pabbajjamarocayittha,",
  "ja509:22.2_0": "Raṭṭhaṁ pahāya naravīraseṭṭho;",
  "ja509:22.3_0": "Ahampi ekā carissāmi loke,",
  "ja509:22.4_0": "Hitvāna kāmāni manoramāni.",
  "ja509:23.1_0": "Rājā ca pabbajjamarocayittha,",
  "ja509:23.2_0": "Raṭṭhaṁ pahāya naravīraseṭṭho;",
  "ja509:23.3_0": "Ahampi ekā carissāmi loke,",
  "ja509:23.4_0": "Hitvāna kāmāni yathodhikāni.",
  "ja509:24.1_0": "Accenti kālā tarayanti rattiyo,",
  "ja509:24.2_0": "Vayoguṇā anupubbaṁ jahanti;",
  "ja509:24.3_0": "Ahampi ekā carissāmi loke,",
  "ja509:24.4_0": "Hitvāna kāmāni manoramāni.",
  "ja509:25.1_0": "Accenti kālā tarayanti rattiyo,",
  "ja509:25.2_0": "Vayoguṇā anupubbaṁ jahanti;",
  "ja509:25.3_0": "Ahampi ekā carissāmi loke,",
  "ja509:25.4_0": "Hitvāna kāmāni yathodhikāni.",
  "ja509:26.1_0": "Accenti kālā tarayanti rattiyo,",
  "ja509:26.2_0": "Vayoguṇā anupubbaṁ jahanti;",
  "ja509:26.3_0": "Ahampi ekā carissāmi loke,",
  "ja509:26.4_0": "Sītibhūtā sabbamaticca saṅgan”ti.",
  "ja509:27.1_0": "Hatthipālajātakaṁ terasamaṁ."
}