{
  "anya-e1007n0:0_0": " Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1007n0:1_0": "Sūrassatīnīti",
  "anya-e1007n0:2_0": "Paṭhamo bhāgo",
  "anya-e1007n0:3_0": "Paṇāmagāthā",
  "anya-e1007n0:4_0": "1.",
  "anya-e1007n0:5_0": " Mukhamhā bhagavantassa,",
  "anya-e1007n0:6_0": "Sugandhakamalā subhā;",
  "anya-e1007n0:7_0": "Sañjātaṁ uttamaṁ vāṇiṁ,",
  "anya-e1007n0:8_0": "Vandāmi vittamānasā.",
  "anya-e1007n0:9_0": "2.",
  "anya-e1007n0:10_0": " Samajjhiṭṭho racissāmi,",
  "anya-e1007n0:11_0": "Dakkhena rājamantinā;",
  "anya-e1007n0:12_0": "Nāmena santena,",
  "anya-e1007n0:13_0": "Nītiṁ lokahitāvahaṁ;",
  "anya-e1007n0:14_0": "Sālaṅkāraṁ sopadesaṁ,",
  "anya-e1007n0:15_0": "Nāmenāhaṁ svarassatiṁ.",
  "anya-e1007n0:16_0": "3.",
  "anya-e1007n0:17_0": " Pekkhantvimaṁ nītiṁ santā,",
  "anya-e1007n0:18_0": "Mañjūsarukkhasannibhaṁ;",
  "anya-e1007n0:19_0": "Nānopadesasaṁpuṇṇaṁ,",
  "anya-e1007n0:20_0": "Sadatthakusalā sadā."
}