{
  "ja282:0.1_0": "Jātaka",
  "ja282:0.2_0": "Tikanipāta",
  "ja282:0.3_0": "Abbhantaravagga",
  "ja282:0.4_0": "2. Seyyajātaka",
  "ja282:1.1_0": "“Seyyaṁso seyyaso hoti,",
  "ja282:1.2_0": "yo seyyamupasevati;",
  "ja282:1.3_0": "Ekena sandhiṁ katvāna,",
  "ja282:1.4_0": "sataṁ vajjhe amocayiṁ.",
  "ja282:2.1_0": "Tasmā sabbena lokena,",
  "ja282:2.2_0": "sandhiṁ katvāna ekato;",
  "ja282:2.3_0": "Pecca saggaṁ nigaccheyya,",
  "ja282:2.4_0": "idaṁ suṇātha kāsiyā”.",
  "ja282:3.1_0": "Idaṁ vatvā mahārājā,",
  "ja282:3.2_0": "Kaṁso bārāṇasiggaho;",
  "ja282:3.3_0": "Dhanuṁ kaṇḍañca nikkhippa,",
  "ja282:3.4_0": "Saṁyamaṁ ajjhupāgamīti.",
  "ja282:4.1_0": "Seyyajātakaṁ dutiyaṁ."
}