{
  "anya-e0605n25:0_0": "Pāṭihāriyadīpanīgāthā",
  "anya-e0605n25:1_0": "175.",
  "anya-e0605n25:2_0": "Brahmassa saddaṁ karavīkabhāṇiṁ,",
  "anya-e0605n25:3_0": "Yathicchitaṁ sāvayituṁ samatthaṁ;",
  "anya-e0605n25:4_0": "Saccaṁ piyaṁ bhūtahitaṁ vadantaṁ,",
  "anya-e0605n25:5_0": "Na pūjaye ko hi naro sacetano.",
  "anya-e0605n25:6_0": "176.",
  "anya-e0605n25:7_0": "Iddhi ca ādesanānusāsanī,",
  "anya-e0605n25:8_0": "Pāṭihīre bhagavā vasī ahu;",
  "anya-e0605n25:9_0": "Katvāna accherasupāṭihīraṁ,",
  "anya-e0605n25:10_0": "Desesi dhammaṁ anukampimaṁ pajaṁ."
}