{
  "tha-ap27:0.1_0": "Therāpadāna",
  "tha-ap27:0.2_0": "Subhūtivagga",
  "tha-ap27:0.3_0": "5. Annasaṁsāvakattheraapadāna",
  "tha-ap27:1.1_0": "“Suvaṇṇavaṇṇaṁ sambuddhaṁ,",
  "tha-ap27:1.2_0": "Gacchantaṁ antarāpaṇe;",
  "tha-ap27:1.3_0": "Kañcanagghiyasaṅkāsaṁ,",
  "tha-ap27:1.4_0": "Bāttiṁsavaralakkhaṇaṁ.",
  "tha-ap27:2.1_0": "Siddhatthaṁ lokapajjotaṁ,",
  "tha-ap27:2.2_0": "appameyyaṁ anopamaṁ;",
  "tha-ap27:2.3_0": "Alatthaṁ paramaṁ pītiṁ,",
  "tha-ap27:2.4_0": "disvā dantaṁ jutindharaṁ.",
  "tha-ap27:3.1_0": "Sambuddhaṁ abhināmetvā,",
  "tha-ap27:3.2_0": "bhojayiṁ taṁ mahāmuniṁ;",
  "tha-ap27:3.3_0": "Mahākāruṇiko loke,",
  "tha-ap27:3.4_0": "anumodi mamaṁ tadā.",
  "tha-ap27:4.1_0": "Tasmiṁ mahākāruṇike,",
  "tha-ap27:4.2_0": "paramassāsakārake;",
  "tha-ap27:4.3_0": "Buddhe cittaṁ pasādetvā,",
  "tha-ap27:4.4_0": "kappaṁ saggamhi modahaṁ.",
  "tha-ap27:5.1_0": "Catunnavutito kappe,",
  "tha-ap27:5.2_0": "yaṁ dānamadadiṁ tadā;",
  "tha-ap27:5.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap27:5.4_0": "bhikkhādānassidaṁ phalaṁ.",
  "tha-ap27:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap27:6.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap27:6.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap27:6.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap27:7.1_0": "Itthaṁ sudaṁ āyasmā annasaṁsāvako thero imā gāthāyo abhāsitthāti.",
  "tha-ap27:8.1_0": "Annasaṁsāvakattherassāpadānaṁ pañcamaṁ."
}