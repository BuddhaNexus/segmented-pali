{
  "ja110:0.1_0": "Jātaka",
  "ja110:0.2_0": "Ekakanipāta",
  "ja110:0.3_0": "Parosatavagga",
  "ja110:0.4_0": "10. Sabbasaṁhārakapañhajātaka",
  "ja110:1.1_0": "“Sabbasaṁhārako natthi,",
  "ja110:1.2_0": "suddhaṁ kaṅgu pavāyati;",
  "ja110:1.3_0": "Alikaṁ bhāyatiyaṁ dhuttī,",
  "ja110:1.4_0": "saccamāha mahallikā”ti.",
  "ja110:2.1_0": "Sabbasaṁhārakapañhajātakaṁ dasamaṁ.",
  "ja110:2.2_0": "Parosatavaggo ekādasamo.",
  "ja110:3.1_0": "Tassuddānaṁ",
  "ja110:4.1_0": "Saparosatatāyitaverī puna,",
  "ja110:4.2_0": "Bhamacakkatha nāgasirivhayano;",
  "ja110:4.3_0": "Sukhakañca vata sippaka bāhiyā,",
  "ja110:4.4_0": "Kuṇḍapūva mahallikakā ca dasāti."
}