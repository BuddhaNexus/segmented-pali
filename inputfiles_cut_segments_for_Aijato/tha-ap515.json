{
  "tha-ap515:0.1_0": "Therāpadāna",
  "tha-ap515:0.2_0": "Phaladāyakavagga",
  "tha-ap515:0.3_0": "3 Kosambaphaliyattheraapadāna",
  "tha-ap515:1.1_0": "“Kakudhaṁ vilasantaṁva,",
  "tha-ap515:1.2_0": "devadevaṁ narāsabhaṁ;",
  "tha-ap515:1.3_0": "Rathiyaṁ paṭipajjantaṁ,",
  "tha-ap515:1.4_0": "kosambaṁ adadiṁ tadā.",
  "tha-ap515:2.1_0": "Ekatiṁse ito kappe,",
  "tha-ap515:2.2_0": "yaṁ phalaṁ adadiṁ tadā;",
  "tha-ap515:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap515:2.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap515:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap515:3.2_0": "…pe…",
  "tha-ap515:3.3_0": "viharāmi anāsavo.",
  "tha-ap515:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap515:4.2_0": "…pe…",
  "tha-ap515:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap515:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap515:5.2_0": "…pe…",
  "tha-ap515:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap515:6.1_0": "Itthaṁ sudaṁ āyasmā kosambaphaliyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap515:7.1_0": "Kosambaphaliyattherassāpadānaṁ tatiyaṁ."
}