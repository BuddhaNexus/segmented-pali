{
  "tha-ap433:0.1_0": "Therāpadāna",
  "tha-ap433:0.2_0": "Ekavihārivagga",
  "tha-ap433:0.3_0": "1 Ekavihārikattheraapadāna",
  "tha-ap433:1.1_0": "“Imamhi bhaddake kappe,",
  "tha-ap433:1.2_0": "brahmabandhu mahāyaso;",
  "tha-ap433:1.3_0": "Kassapo nāma gottena,",
  "tha-ap433:1.4_0": "uppajji vadataṁ varo.",
  "tha-ap433:2.1_0": "Nippapañco nirālambo,",
  "tha-ap433:2.2_0": "ākāsasamamānaso;",
  "tha-ap433:2.3_0": "Suññatābahulo tādī,",
  "tha-ap433:2.4_0": "animittarato vasī.",
  "tha-ap433:3.1_0": "Asaṅgacitto nikleso,",
  "tha-ap433:3.2_0": "asaṁsaṭṭho kule gaṇe;",
  "tha-ap433:3.3_0": "Mahākāruṇiko vīro,",
  "tha-ap433:3.4_0": "vinayopāyakovido.",
  "tha-ap433:4.1_0": "Uyyutto parakiccesu,",
  "tha-ap433:4.2_0": "vinayanto sadevake;",
  "tha-ap433:4.3_0": "Nibbānagamanaṁ maggaṁ,",
  "tha-ap433:4.4_0": "gatiṁ paṅkavisosanaṁ.",
  "tha-ap433:5.1_0": "Amataṁ paramassādaṁ,",
  "tha-ap433:5.2_0": "jarāmaccunivāraṇaṁ;",
  "tha-ap433:5.3_0": "Mahāparisamajjhe so,",
  "tha-ap433:5.4_0": "nisinno lokatārako.",
  "tha-ap433:6.1_0": "Karavīkaruto nātho,",
  "tha-ap433:6.2_0": "brahmaghoso tathāgato;",
  "tha-ap433:6.3_0": "Uddharanto mahāduggā,",
  "tha-ap433:6.4_0": "vippanaṭṭhe anāyake.",
  "tha-ap433:7.1_0": "Desento virajaṁ dhammaṁ,",
  "tha-ap433:7.2_0": "diṭṭho me lokanāyako;",
  "tha-ap433:7.3_0": "Tassa dhammaṁ suṇitvāna,",
  "tha-ap433:7.4_0": "pabbajiṁ anagāriyaṁ.",
  "tha-ap433:8.1_0": "Pabbajitvā tadāpāhaṁ,",
  "tha-ap433:8.2_0": "cintento jinasāsanaṁ;",
  "tha-ap433:8.3_0": "Ekakova vane ramme,",
  "tha-ap433:8.4_0": "vasiṁ saṁsaggapīḷito.",
  "tha-ap433:9.1_0": "Sakkāyavūpakāso me,",
  "tha-ap433:9.2_0": "hetubhūto mamābhavī;",
  "tha-ap433:9.3_0": "Manaso vūpakāsassa,",
  "tha-ap433:9.4_0": "saṁsaggabhayadassino.",
  "tha-ap433:10.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap433:10.2_0": "bhavā sabbe samūhatā;",
  "tha-ap433:10.3_0": "Nāgova bandhanaṁ chetvā,",
  "tha-ap433:10.4_0": "viharāmi anāsavo.",
  "tha-ap433:11.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap433:11.2_0": "mama buddhassa santike;",
  "tha-ap433:11.3_0": "Tisso vijjā anuppattā,",
  "tha-ap433:11.4_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap433:12.1_0": "Paṭisambhidā catasso,",
  "tha-ap433:12.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap433:12.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap433:12.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap433:13.1_0": "Itthaṁ sudaṁ āyasmā ekavihāriko thero imā gāthāyo abhāsitthāti.",
  "tha-ap433:14.1_0": "Ekavihārikattherassāpadānaṁ paṭhamaṁ."
}