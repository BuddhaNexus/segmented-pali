{
  "tha-ap526:0.1_0": "Therāpadāna",
  "tha-ap526:0.2_0": "Tiṇadāyakavagga",
  "tha-ap526:0.3_0": "4. Abbhañjanadāyakattheraapadāna",
  "tha-ap526:1.1_0": "“Nagare bandhumatiyā,",
  "tha-ap526:1.2_0": "rājuyyāne vasāmahaṁ;",
  "tha-ap526:1.3_0": "Cammavāsī tadā āsiṁ,",
  "tha-ap526:1.4_0": "kamaṇḍaludharo ahaṁ.",
  "tha-ap526:2.1_0": "Addasaṁ vimalaṁ buddhaṁ,",
  "tha-ap526:2.2_0": "sayambhuṁ aparājitaṁ;",
  "tha-ap526:2.3_0": "Padhānaṁ pahitattaṁ taṁ,",
  "tha-ap526:2.4_0": "jhāyiṁ jhānarataṁ vasiṁ.",
  "tha-ap526:3.1_0": "Sabbakāmasamiddhiñca,",
  "tha-ap526:3.2_0": "oghatiṇṇamanāsavaṁ;",
  "tha-ap526:3.3_0": "Disvā pasanno sumano,",
  "tha-ap526:3.4_0": "abbhañjanamadāsahaṁ.",
  "tha-ap526:4.1_0": "Ekanavutito kappe,",
  "tha-ap526:4.2_0": "yaṁ dānamadadiṁ tadā;",
  "tha-ap526:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap526:4.4_0": "abbhañjanassidaṁ phalaṁ.",
  "tha-ap526:5.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap526:5.2_0": "…pe…",
  "tha-ap526:5.3_0": "viharāmi anāsavo.",
  "tha-ap526:6.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap526:6.2_0": "…pe…",
  "tha-ap526:6.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap526:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap526:7.2_0": "…pe…",
  "tha-ap526:7.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap526:8.1_0": "Itthaṁ sudaṁ āyasmā abbhañjanadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap526:9.1_0": "Abbhañjanadāyakattherassāpadānaṁ catutthaṁ."
}