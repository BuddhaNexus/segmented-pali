{
  "anya-e1009n11:0_0": "Pakiṇṇaka-niddesa",
  "anya-e1009n11:1_0": "253.",
  "anya-e1009n11:2_0": "Kumudaṁ ko pabodhayi,",
  "anya-e1009n11:3_0": "Nātho ravindu paṇḍito;",
  "anya-e1009n11:4_0": "Kamalaṁ ko kumudaṁ ko,",
  "anya-e1009n11:5_0": "Narapaṁ ko pabodhayi.",
  "anya-e1009n11:6_0": "254.",
  "anya-e1009n11:7_0": "Cittena niyyati loko,",
  "anya-e1009n11:8_0": "Cittena parikassati;",
  "anya-e1009n11:9_0": "Cittassa eka-dhammassa,",
  "anya-e1009n11:10_0": "Sabbeva vasamanvagū.",
  "anya-e1009n11:11_0": "255.",
  "anya-e1009n11:12_0": "Samaṇo rājānurājā,",
  "anya-e1009n11:13_0": "Senāpati mahā-matto;",
  "anya-e1009n11:14_0": "Dhammaṭṭho paṇḍito disvā,",
  "anya-e1009n11:15_0": "Paccakkhatthaṁ na kāriyā.",
  "anya-e1009n11:16_0": "256.",
  "anya-e1009n11:17_0": "Dīpo nava-disaṁ tejo,",
  "anya-e1009n11:18_0": "Na heṭṭhā ca tathā sakaṁ;",
  "anya-e1009n11:19_0": "Para-vajjaṁ vidū passe,",
  "anya-e1009n11:20_0": "Saka-vajjaṁpi passatu.",
  "anya-e1009n11:21_0": "257.",
  "anya-e1009n11:22_0": "Sa-phalaṁ paṇḍito loke,",
  "anya-e1009n11:23_0": "Sa-kāraṇaṁ vacaṁ bhaṇe;",
  "anya-e1009n11:24_0": "A-kāraṇaṁphalaṁ bālo,",
  "anya-e1009n11:25_0": "Idaṁ ubhaya-lakkhaṇaṁ.",
  "anya-e1009n11:26_0": "258.",
  "anya-e1009n11:27_0": "Tassa vācāya jāneyya,",
  "anya-e1009n11:28_0": "Kuṭilaṁ bāla-paṇḍitaṁ;",
  "anya-e1009n11:29_0": "Vācā-rūpaṁ mittaṁ kare,",
  "anya-e1009n11:30_0": "Vācā-rūpaṁ dhuvaṁ jahe.",
  "anya-e1009n11:31_0": "259.",
  "anya-e1009n11:32_0": "Duccintitassa cintā ca,",
  "anya-e1009n11:33_0": "Dubbhāsitassa bhāsanā;",
  "anya-e1009n11:34_0": "Dukkammassa katañcāti,",
  "anya-e1009n11:35_0": "Etaṁ bālassa lakkhaṇaṁ.",
  "anya-e1009n11:36_0": "260.",
  "anya-e1009n11:37_0": "Su-cintitassa cintā ca,",
  "anya-e1009n11:38_0": "Su-bhāsitassa bhāsanā;",
  "anya-e1009n11:39_0": "Su-kammassa katañcāti,",
  "anya-e1009n11:40_0": "Etaṁ dhīrassa lakkhaṇaṁ.",
  "anya-e1009n11:41_0": "261.",
  "anya-e1009n11:42_0": "A-nayaṁ nayati dummedho,",
  "anya-e1009n11:43_0": "A-dhurāyaṁ ni-yuñjati;",
  "anya-e1009n11:44_0": "Dunnayo seyyaso hoti,",
  "anya-e1009n11:45_0": "Sammā vutto pakuppati;",
  "anya-e1009n11:46_0": "Vinayaṁ so na jānāti,",
  "anya-e1009n11:47_0": "Sādhu tassa a-dassanaṁ.",
  "anya-e1009n11:48_0": "262.",
  "anya-e1009n11:49_0": "Nayaṁ nayati medhāvī,",
  "anya-e1009n11:50_0": "A-dhurāyaṁ na yuñjati;",
  "anya-e1009n11:51_0": "Su-nayo seyyaso hoti,",
  "anya-e1009n11:52_0": "Sammā vutto na kuppati;",
  "anya-e1009n11:53_0": "Vinayaṁ so pajānāti,",
  "anya-e1009n11:54_0": "Sādhu tena samāgamo.",
  "anya-e1009n11:55_0": "263.",
  "anya-e1009n11:56_0": "A-nāyakā vinassanti,",
  "anya-e1009n11:57_0": "Nassanti bahu-nāyakā;",
  "anya-e1009n11:58_0": "Thī-nāyakā vinassanti,",
  "anya-e1009n11:59_0": "Nassanti susu-nāyakā.",
  "anya-e1009n11:60_0": "264.",
  "anya-e1009n11:61_0": "Jeṭṭho kammesu nīcānaṁ,",
  "anya-e1009n11:62_0": "Jānaṁjānaṁva ācare;",
  "anya-e1009n11:63_0": "A-jānevaṁ kare jānaṁ,",
  "anya-e1009n11:64_0": "Nīco eti bhayaṁ piyaṁ.",
  "anya-e1009n11:65_0": "265.",
  "anya-e1009n11:66_0": "Kammaṁ dujjana-sāruppaṁ,",
  "anya-e1009n11:67_0": "Dudhā sujana-sāruppaṁ;",
  "anya-e1009n11:68_0": "Dujjanaṁ tesu dukkamme,",
  "anya-e1009n11:69_0": "Su-kamme sujanaṁ icche.",
  "anya-e1009n11:70_0": "266.",
  "anya-e1009n11:71_0": "Paṇḍito verī bālo ca,",
  "anya-e1009n11:72_0": "Dujjayo bāla-verito;",
  "anya-e1009n11:73_0": "Paṇḍitaṁ-verī pamādena,",
  "anya-e1009n11:74_0": "Na taṁ jayo hi sabbadā.",
  "anya-e1009n11:75_0": "267.",
  "anya-e1009n11:76_0": "Guyhassa hi guyhameva sādhu,",
  "anya-e1009n11:77_0": "Na hi guyhassa pasatthamāvi-kammaṁ;",
  "anya-e1009n11:78_0": "A-nipphannatāya saheyya dhīro,",
  "anya-e1009n11:79_0": "Nipphannatthova yathā-sukhaṁ bhaṇeyya.",
  "anya-e1009n11:80_0": "268.",
  "anya-e1009n11:81_0": "Guyhamatthaṁ na vivareyya,",
  "anya-e1009n11:82_0": "Rakkheyya naṁ yathā nidhiṁ;",
  "anya-e1009n11:83_0": "Na hī pātukato sādhu,",
  "anya-e1009n11:84_0": "Guyho attho pajānatā.",
  "anya-e1009n11:85_0": "269.",
  "anya-e1009n11:86_0": "Thiyā guyhaṁ na saṁseyya,",
  "anya-e1009n11:87_0": "A-mittassa ca paṇḍito;",
  "anya-e1009n11:88_0": "Yo cāmisena saṁhīro,",
  "anya-e1009n11:89_0": "Hadaya-ttheno ca yo naro.",
  "anya-e1009n11:90_0": "270.",
  "anya-e1009n11:91_0": "Vivicca bhāseyya divā rahassaṁ,",
  "anya-e1009n11:92_0": "Rattiṁ giraṁ nāti-velaṁ pamuñce;",
  "anya-e1009n11:93_0": "Upassutikā hi suṇanti mantaṁ,",
  "anya-e1009n11:94_0": "Tasmā hi manto khippamupeti bhedaṁ.",
  "anya-e1009n11:95_0": "271.",
  "anya-e1009n11:96_0": "Na pakāsati guyhaṁ yo,",
  "anya-e1009n11:97_0": "So guyhaṁ paṭiguyhati;",
  "anya-e1009n11:98_0": "Bhayesu na jahe kicce,",
  "anya-e1009n11:99_0": "Su-mittonucaro bhave.",
  "anya-e1009n11:100_0": "272.",
  "anya-e1009n11:101_0": "Karoti dukkaraṁ sādhuṁ,",
  "anya-e1009n11:102_0": "Ujuṁ khamati dukkhamaṁ;",
  "anya-e1009n11:103_0": "Duddadaṁ sāmaṁ dadāti,",
  "anya-e1009n11:104_0": "Yo su-mitto have bhave.",
  "anya-e1009n11:105_0": "273.",
  "anya-e1009n11:106_0": "Piya-vācā sadā mitto,",
  "anya-e1009n11:107_0": "Piya-vatthuṁ na yācanā;",
  "anya-e1009n11:108_0": "Icchāgatena dānena,",
  "anya-e1009n11:109_0": "Su-daḷho su-ppiyo have;",
  "anya-e1009n11:110_0": "Tadaṅgato ca hīnena,",
  "anya-e1009n11:111_0": "A-ppiyo bhijjano bhave.",
  "anya-e1009n11:112_0": "274.",
  "anya-e1009n11:113_0": "Dehīti yācane hirī,",
  "anya-e1009n11:114_0": "Sirī ca kāya-devatā;",
  "anya-e1009n11:115_0": "Palāyanti siricchito,",
  "anya-e1009n11:116_0": "Na yāce para-santakaṁ.",
  "anya-e1009n11:117_0": "275.",
  "anya-e1009n11:118_0": "Svāno laddhāna nimmaṁsaṁ,",
  "anya-e1009n11:119_0": "Aṭṭhiṁ tuṭṭho pamodati;",
  "anya-e1009n11:120_0": "Sakantikaṁ migaṁ sīho,",
  "anya-e1009n11:121_0": "Hitvā hatthiṁnudhāvati.",
  "anya-e1009n11:122_0": "276.",
  "anya-e1009n11:123_0": "Evaṁ chandānurūpena,",
  "anya-e1009n11:124_0": "Jano āsīsate phalaṁ;",
  "anya-e1009n11:125_0": "Mahā chandā mahantaṁva,",
  "anya-e1009n11:126_0": "Hīnaṁva hīna-chandakā.",
  "anya-e1009n11:127_0": "277.",
  "anya-e1009n11:128_0": "Nānā-chandā mahārāja,",
  "anya-e1009n11:129_0": "Ekāgāre vasāmase;",
  "anya-e1009n11:130_0": "Ahaṁ gāma-varaṁ icche,",
  "anya-e1009n11:131_0": "Brāhmaṇī ca gavaṁ sataṁ.",
  "anya-e1009n11:132_0": "278.",
  "anya-e1009n11:133_0": "Putto ca ājañña-rathaṁ,",
  "anya-e1009n11:134_0": "Kaññā ca maṇi-kuṇḍalaṁ;",
  "anya-e1009n11:135_0": "Yā cesā puṇṇakā jammī,",
  "anya-e1009n11:136_0": "Ujukkhalaṁbhi-kaṅkhati.",
  "anya-e1009n11:137_0": "279.",
  "anya-e1009n11:138_0": "Ṭhānaṁ mitte dhane kamme,",
  "anya-e1009n11:139_0": "Satussāhe su-labbhitaṁ;",
  "anya-e1009n11:140_0": "Taṁ daḷhaṁ dukkaraṁ kare,",
  "anya-e1009n11:141_0": "Paññā-sati-samādhinā.",
  "anya-e1009n11:142_0": "280.",
  "anya-e1009n11:143_0": "Bhesajje vihite suddha,",
  "anya-e1009n11:144_0": "Buddhādi-ratanattaye;",
  "anya-e1009n11:145_0": "Pasādamācare niccaṁ,",
  "anya-e1009n11:146_0": "Sajjane sa-guṇepi ca.",
  "anya-e1009n11:147_0": "281.",
  "anya-e1009n11:148_0": "Rājā raṭṭhena dhātuso,",
  "anya-e1009n11:149_0": "Bālo pāpehi dummano,",
  "anya-e1009n11:150_0": "Alaṅkārena itthīpi,",
  "anya-e1009n11:151_0": "Kāmehi ca na tappati.",
  "anya-e1009n11:152_0": "282.",
  "anya-e1009n11:153_0": "Appiccho ca dhutaṅgena,",
  "anya-e1009n11:154_0": "Āraddho vīriyena hi;",
  "anya-e1009n11:155_0": "Visārado parisāya,",
  "anya-e1009n11:156_0": "Pariccāgena dāyako;",
  "anya-e1009n11:157_0": "Savanena su-dhammaṁpi,",
  "anya-e1009n11:158_0": "Na tappativa paṇḍito.",
  "anya-e1009n11:159_0": "283.",
  "anya-e1009n11:160_0": "Jeṭṭhassa sitaṁ hasitaṁ,",
  "anya-e1009n11:161_0": "Majjhassa madhurassaraṁ;",
  "anya-e1009n11:162_0": "Loke aṁsa-siro-kampaṁ,",
  "anya-e1009n11:163_0": "Jammassa apa-hassitaṁ;",
  "anya-e1009n11:164_0": "Etesaṁ ati-hassitaṁ,",
  "anya-e1009n11:165_0": "Hāso hoti yathākkamaṁ.",
  "anya-e1009n11:166_0": "284.",
  "anya-e1009n11:167_0": "Natthi duṭṭhe nayo atthi,",
  "anya-e1009n11:168_0": "Na dhammo na su-bhāsitaṁ;",
  "anya-e1009n11:169_0": "Nikkamaṁ duṭṭhe yuñjeyya,",
  "anya-e1009n11:170_0": "So hi sabbhiṁ na rañjati.",
  "anya-e1009n11:171_0": "285.",
  "anya-e1009n11:172_0": "Dullabhaṁ pakatiṁ vācaṁ,",
  "anya-e1009n11:173_0": "Dullabho khemako suto;",
  "anya-e1009n11:174_0": "Dullabhā sadisī jāyā,",
  "anya-e1009n11:175_0": "Dullabho sa-jano piyo.",
  "anya-e1009n11:176_0": "286.",
  "anya-e1009n11:177_0": "Dhajo rathassa paññāṇaṁ,",
  "anya-e1009n11:178_0": "Dhūmo paññāṇamaggino;",
  "anya-e1009n11:179_0": "Rājā raṭṭhassa paññāṇaṁ,",
  "anya-e1009n11:180_0": "Bhattā paññāṇamitthiyā.",
  "anya-e1009n11:181_0": "287.",
  "anya-e1009n11:182_0": "Dunnāriyā kulaṁ suddhaṁ,",
  "anya-e1009n11:183_0": "Putto nassati lālanā;",
  "anya-e1009n11:184_0": "Samiddhi a-nayā bandhu,",
  "anya-e1009n11:185_0": "Pavāsā madanā hirī.",
  "anya-e1009n11:186_0": "288.",
  "anya-e1009n11:187_0": "Mātā pitā ca puttānaṁ,",
  "anya-e1009n11:188_0": "Novāde bahu-sāsannaṁ;",
  "anya-e1009n11:189_0": "Paṇḍitā mātaro appaṁ,",
  "anya-e1009n11:190_0": "Vadeyyuṁ vajja-dīpanaṁ.",
  "anya-e1009n11:191_0": "289.",
  "anya-e1009n11:192_0": "Lāḷaye pañcha-vassāni,",
  "anya-e1009n11:193_0": "Dasa-vassāni tāḷaye;",
  "anya-e1009n11:194_0": "Patte tu soḷase vasse,",
  "anya-e1009n11:195_0": "Puttaṁ mittaṁvadācare.",
  "anya-e1009n11:196_0": "290.",
  "anya-e1009n11:197_0": "Lālane dhītaraṁ dosā,",
  "anya-e1009n11:198_0": "Pālane bahavo guṇā;",
  "anya-e1009n11:199_0": "Dhītuyā kiriyaṁ niccaṁ,",
  "anya-e1009n11:200_0": "Passantu suṭṭhu mātaro.",
  "anya-e1009n11:201_0": "Iti pakiṇṇaka-niddeso nāma",
  "anya-e1009n11:202_0": "Sattamā paricchedo."
}