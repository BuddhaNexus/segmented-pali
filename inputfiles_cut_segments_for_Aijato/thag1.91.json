{
  "thag1.91:0.1_0": "Theragāthā",
  "thag1.91:0.2_0": "Ekakanipāta",
  "thag1.91:0.3_0": "Dasamavagga",
  "thag1.91:0.4_0": "1.91. Paripuṇṇakattheragāthā",
  "thag1.91:1.1_0": "“Na tathā mataṁ satarasaṁ,",
  "thag1.91:1.2_0": "Sudhannaṁ yaṁ mayajja paribhuttaṁ;",
  "thag1.91:1.3_0": "Aparimitadassinā gotamena,",
  "thag1.91:1.4_0": "Buddhena desito dhammo”ti.",
  "thag1.91:1.5_0": "… Paripuṇṇako thero …."
}