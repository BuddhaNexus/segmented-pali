{
  "ja210:0.1_0": "Jātaka",
  "ja210:0.2_0": "Dukanipāta",
  "ja210:0.3_0": "Nataṁdaḷhavagga",
  "ja210:0.4_0": "10. Kandagalakajātaka",
  "ja210:1.1_0": "“Ambho ko nāma yaṁ rukkho,",
  "ja210:1.2_0": "sinnapatto sakaṇṭako;",
  "ja210:1.3_0": "Yattha ekappahārena,",
  "ja210:1.4_0": "uttamaṅgaṁ vibhijjitaṁ”.",
  "ja210:2.1_0": "“Acāri vatāyaṁ vitudaṁ vanāni,",
  "ja210:2.2_0": "Kaṭṭhaṅgarukkhesu asārakesu;",
  "ja210:2.3_0": "Athāsadā khadiraṁ jātasāraṁ,",
  "ja210:2.4_0": "Yatthabbhidā garuḷo uttamaṅgan”ti.",
  "ja210:3.1_0": "Kandagalakajātakaṁ dasamaṁ.",
  "ja210:3.2_0": "Nataṁdaḷhavaggo chaṭṭho.",
  "ja210:4.1_0": "Tassuddānaṁ",
  "ja210:5.1_0": "Daḷhabandhana haṁsavaro ca puna,",
  "ja210:5.2_0": "Virūpakkha saviṭṭhaka macchavaro;",
  "ja210:5.3_0": "Sakuruṅga saassaka ambavaro,",
  "ja210:5.4_0": "Puna kukkuṭako garuḷena dasāti."
}