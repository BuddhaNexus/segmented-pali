{
  "ja439:0.1_0": "Jātaka",
  "ja439:0.2_0": "Dasakanipāta",
  "ja439:0.3_0": "Catudvāravagga",
  "ja439:0.4_0": "1. Catudvārajātaka",
  "ja439:1.1_0": "“Catudvāramidaṁ nagaraṁ,",
  "ja439:1.2_0": "āyasaṁ daḷhapākāraṁ;",
  "ja439:1.3_0": "Oruddhapaṭiruddhosmi,",
  "ja439:1.4_0": "kiṁ pāpaṁ pakataṁ mayā”.",
  "ja439:2.1_0": "“Sabbe apihitā dvārā,",
  "ja439:2.2_0": "oruddhosmi yathā dijo;",
  "ja439:2.3_0": "Kimādhikaraṇaṁ yakkha,",
  "ja439:2.4_0": "cakkābhinihato ahaṁ.",
  "ja439:3.1_0": "Laddhā satasahassāni,",
  "ja439:3.2_0": "atirekāni vīsati;",
  "ja439:3.3_0": "Anukampakānaṁ ñātīnaṁ,",
  "ja439:3.4_0": "vacanaṁ samma nākari.",
  "ja439:4.1_0": "Laṅghiṁ samuddaṁ pakkhandi,",
  "ja439:4.2_0": "sāgaraṁ appasiddhikaṁ;",
  "ja439:4.3_0": "Catubbhi aṭṭhajjhagamā,",
  "ja439:4.4_0": "aṭṭhāhipi ca soḷasa.",
  "ja439:5.1_0": "Soḷasāhi ca bāttiṁsa,",
  "ja439:5.2_0": "atricchaṁ cakkamāsado;",
  "ja439:5.3_0": "Icchāhatassa posassa,",
  "ja439:5.4_0": "cakkaṁ bhamati matthake.",
  "ja439:6.1_0": "Uparivisālā duppūrā,",
  "ja439:6.2_0": "icchā visaṭagāminī;",
  "ja439:6.3_0": "Ye ca taṁ anugijjhanti,",
  "ja439:6.4_0": "te honti cakkadhārino.",
  "ja439:7.1_0": "Bahubhaṇḍaṁ avahāya,",
  "ja439:7.2_0": "maggaṁ appaṭivekkhiya;",
  "ja439:7.3_0": "Yesañcetaṁ asaṅkhātaṁ,",
  "ja439:7.4_0": "te honti cakkadhārino.",
  "ja439:8.1_0": "Kammaṁ samekkhe vipulañca bhogaṁ,",
  "ja439:8.2_0": "Icchaṁ na seveyya anatthasaṁhitaṁ;",
  "ja439:8.3_0": "Kareyya vākyaṁ anukampakānaṁ,",
  "ja439:8.4_0": "Taṁ tādisaṁ nātivatteyya cakkaṁ”.",
  "ja439:9.1_0": "“Kīvaciraṁ nu me yakkha,",
  "ja439:9.2_0": "cakkaṁ sirasi ṭhassati;",
  "ja439:9.3_0": "Kati vassasahassāni,",
  "ja439:9.4_0": "taṁ me akkhāhi pucchito”.",
  "ja439:10.1_0": "“Atisaro paccasaro,",
  "ja439:10.2_0": "mittavinda suṇohi me;",
  "ja439:10.3_0": "Cakkaṁ te sirasi māviddhaṁ,",
  "ja439:10.4_0": "na taṁ jīvaṁ pamokkhasī”ti.",
  "ja439:11.1_0": "Catudvārajātakaṁ paṭhamaṁ."
}