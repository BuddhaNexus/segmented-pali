{
  "ja324:0.1_0": "Jātaka",
  "ja324:0.2_0": "Catukkanipāta",
  "ja324:0.3_0": "Kuṭidūsakavagga",
  "ja324:0.4_0": "4. Cammasāṭakajātaka",
  "ja324:1.1_0": "“Kalyāṇarūpo vatayaṁ catuppado,",
  "ja324:1.2_0": "Subhaddako ceva supesalo ca;",
  "ja324:1.3_0": "Yo brāhmaṇaṁ jātimantūpapannaṁ,",
  "ja324:1.4_0": "Apacāyati meṇḍavaro yasassī”.",
  "ja324:2.1_0": "“Mā brāhmaṇa ittaradassanena,",
  "ja324:2.2_0": "Vissāsamāpajji catuppadassa;",
  "ja324:2.3_0": "Daḷhappahāraṁ abhikaṅkhamāno,",
  "ja324:2.4_0": "Avasakkatī dassati suppahāraṁ”.",
  "ja324:3.1_0": "Ūruṭṭhi bhaggaṁ pavaṭṭito khāribhāro,",
  "ja324:3.2_0": "Sabbañca bhaṇḍaṁ brāhmaṇassa bhinnaṁ;",
  "ja324:3.3_0": "Ubhopi bāhā paggayha kandati,",
  "ja324:3.4_0": "“Abhidhāvatha haññate brahmacārī.",
  "ja324:4.1_0": "Evaṁ so nihato seti,",
  "ja324:4.2_0": "yo apūjaṁ pasaṁsati;",
  "ja324:4.3_0": "Yathāhamajja pahato,",
  "ja324:4.4_0": "hato meṇḍena dummatī”ti.",
  "ja324:5.1_0": "Cammasāṭakajātakaṁ catutthaṁ."
}