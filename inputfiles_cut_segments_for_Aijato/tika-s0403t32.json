{
  "tika-s0403t32:0_0": "10. Ānisaṁsavaggo",
  "tika-s0403t32:1_0": "1-11. Pātubhāvasuttādivaṇṇanā",
  "tika-s0403t32:2_0": "96-106.",
  "tika-s0403t32:2_1": "Dasamassa paṭhamādīsu natthi vattabbaṁ.",
  "tika-s0403t32:2_2": "Aṭṭhame mettā etassa atthīti mettāvā, tassa bhāvo mettāvatā, mettāpaṭipatti, tāya.",
  "tika-s0403t32:2_3": "Sā pana mettāvatā mettāvasena pāricariyāti āha ‘‘mettāyuttāya pāricariyāyā’’ti.",
  "tika-s0403t32:2_4": "Paricaranti vippakatabrahmacariyattā.",
  "tika-s0403t32:2_5": "Pariciṇṇasatthukena sāvakena nāma satthuno yāva dhammena kātabbā pāricariyā, tāya sammadeva sampāditattā.",
  "tika-s0403t32:2_6": "Navamādīni uttānatthāni.",
  "tika-s0403t32:3_0": "Pātubhāvasuttādivaṇṇanā niṭṭhitā.",
  "tika-s0403t32:4_0": "Ānisaṁsavaggavaṇṇanā niṭṭhitā.",
  "tika-s0403t32:5_0": "107-116. Ekādasamavaggo uttānatthoyeva.",
  "tika-s0403t32:6_0": "Iti manorathapūraṇiyā aṅguttaranikāya-aṭṭhakathāya",
  "tika-s0403t32:7_0": "Chakkanipātavaṇṇanāya anuttānatthadīpanā samattā.",
  "tika-s0403t32:8_0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "tika-s0403t32:9_0": "Aṅguttaranikāye",
  "tika-s0403t32:10_0": "Sattakanipāta-ṭīkā",
  "tika-s0403t32:11_0": "1. Paṭhamapaṇṇāsakaṁ"
}