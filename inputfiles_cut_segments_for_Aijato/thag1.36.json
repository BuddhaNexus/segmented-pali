{
  "thag1.36:0.1_0": "Theragāthā",
  "thag1.36:0.2_0": "Ekakanipāta",
  "thag1.36:0.3_0": "Catutthavagga",
  "thag1.36:0.4_0": "1.36. Kumāputtattheragāthā",
  "thag1.36:1.1_0": "“Sādhu sutaṁ sādhu caritakaṁ,",
  "thag1.36:1.2_0": "Sādhu sadā aniketavihāro;",
  "thag1.36:1.3_0": "Atthapucchanaṁ padakkhiṇakammaṁ,",
  "thag1.36:1.4_0": "Etaṁ sāmaññamakiñcanassā”ti.",
  "thag1.36:1.5_0": "… Kumāputto thero …."
}