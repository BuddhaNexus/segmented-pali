{
  "tha-ap479:0.1_0": "Therāpadāna",
  "tha-ap479:0.2_0": "Naḷamālivagga",
  "tha-ap479:0.3_0": "7. Giripunnāgiyattheraapadāna",
  "tha-ap479:1.1_0": "“Sobhito nāma sambuddho,",
  "tha-ap479:1.2_0": "cittakūṭe vasī tadā;",
  "tha-ap479:1.3_0": "Gahetvā giripunnāgaṁ,",
  "tha-ap479:1.4_0": "sayambhuṁ abhipūjayiṁ.",
  "tha-ap479:2.1_0": "Catunnavutito kappe,",
  "tha-ap479:2.2_0": "yaṁ buddhamabhipūjayiṁ;",
  "tha-ap479:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap479:2.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap479:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap479:3.2_0": "…pe…",
  "tha-ap479:3.3_0": "viharāmi anāsavo.",
  "tha-ap479:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap479:4.2_0": "…pe…",
  "tha-ap479:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap479:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap479:5.2_0": "…pe…",
  "tha-ap479:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap479:6.1_0": "Itthaṁ sudaṁ āyasmā giripunnāgiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap479:7.1_0": "Giripunnāgiyattherassāpadānaṁ sattamaṁ."
}