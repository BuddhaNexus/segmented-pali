{
  "tha-ap98:0.1_0": "Therāpadāna",
  "tha-ap98:0.2_0": "Sudhāvagga",
  "tha-ap98:0.3_0": "6. Tipupphiyattheraapadāna",
  "tha-ap98:1.1_0": "“Migaluddo pure āsiṁ,",
  "tha-ap98:1.2_0": "araññe kānane ahaṁ;",
  "tha-ap98:1.3_0": "Pāṭaliṁ haritaṁ disvā,",
  "tha-ap98:1.4_0": "tīṇi pupphāni okiriṁ.",
  "tha-ap98:2.1_0": "Patitapattāni gaṇhitvā,",
  "tha-ap98:2.2_0": "bahi chaḍḍesahaṁ tadā;",
  "tha-ap98:2.3_0": "Antosuddhaṁ bahisuddhaṁ,",
  "tha-ap98:2.4_0": "suvimuttaṁ anāsavaṁ.",
  "tha-ap98:3.1_0": "Sammukhā viya sambuddhaṁ,",
  "tha-ap98:3.2_0": "vipassiṁ lokanāyakaṁ;",
  "tha-ap98:3.3_0": "Pāṭaliṁ abhivādetvā,",
  "tha-ap98:3.4_0": "tattha kālaṅkato ahaṁ.",
  "tha-ap98:4.1_0": "Ekanavutito kappe,",
  "tha-ap98:4.2_0": "yaṁ bodhimabhipūjayiṁ;",
  "tha-ap98:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap98:4.4_0": "bodhipūjāyidaṁ phalaṁ.",
  "tha-ap98:5.1_0": "Samantapāsādikā nāma,",
  "tha-ap98:5.2_0": "terasāsiṁsu rājino;",
  "tha-ap98:5.3_0": "Ito tettiṁsakappamhi,",
  "tha-ap98:5.4_0": "cakkavattī mahabbalā.",
  "tha-ap98:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap98:6.2_0": "…pe…",
  "tha-ap98:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap98:7.1_0": "Itthaṁ sudaṁ āyasmā tipupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap98:8.1_0": "Tipupphiyattherassāpadānaṁ chaṭṭhaṁ."
}