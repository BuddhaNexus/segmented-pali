{
  "ja354:0.1_0": "Jātaka",
  "ja354:0.2_0": "Pañcakanipāta",
  "ja354:0.3_0": "Maṇikuṇḍalavagga",
  "ja354:0.4_0": "4. Uragajātaka",
  "ja354:1.1_0": "“Uragova tacaṁ jiṇṇaṁ,",
  "ja354:1.2_0": "hitvā gacchati saṁ tanuṁ;",
  "ja354:1.3_0": "Evaṁ sarīre nibbhoge,",
  "ja354:1.4_0": "pete kālaṅkate sati.",
  "ja354:2.1_0": "Ḍayhamāno na jānāti,",
  "ja354:2.2_0": "ñātīnaṁ paridevitaṁ;",
  "ja354:2.3_0": "Tasmā etaṁ na socāmi,",
  "ja354:2.4_0": "gato so tassa yā gati”.",
  "ja354:3.1_0": "“Anavhito tato āgā,",
  "ja354:3.2_0": "ananuññāto ito gato;",
  "ja354:3.3_0": "Yathāgato tathā gato,",
  "ja354:3.4_0": "tattha kā paridevanā.",
  "ja354:4.1_0": "Ḍayhamāno na jānāti,",
  "ja354:4.2_0": "ñātīnaṁ paridevitaṁ;",
  "ja354:4.3_0": "Tasmā etaṁ na socāmi,",
  "ja354:4.4_0": "gato so tassa yā gati”.",
  "ja354:5.1_0": "“Sace rode kisā assaṁ,",
  "ja354:5.2_0": "tassā me kiṁ phalaṁ siyā;",
  "ja354:5.3_0": "Ñātimittasuhajjānaṁ,",
  "ja354:5.4_0": "bhiyyo no aratī siyā.",
  "ja354:6.1_0": "Ḍayhamāno na jānāti,",
  "ja354:6.2_0": "ñātīnaṁ paridevitaṁ;",
  "ja354:6.3_0": "Tasmā etaṁ na socāmi,",
  "ja354:6.4_0": "gato so tassa yā gati”.",
  "ja354:7.1_0": "“Yathāpi dārako candaṁ,",
  "ja354:7.2_0": "gacchantamanurodati;",
  "ja354:7.3_0": "Evaṁsampadamevetaṁ,",
  "ja354:7.4_0": "yo petamanusocati.",
  "ja354:8.1_0": "Ḍayhamāno na jānāti,",
  "ja354:8.2_0": "ñātīnaṁ paridevitaṁ;",
  "ja354:8.3_0": "Tasmā etaṁ na socāmi,",
  "ja354:8.4_0": "gato so tassa yā gati”.",
  "ja354:9.1_0": "“Yathāpi udakakumbho,",
  "ja354:9.2_0": "bhinno appaṭisandhiyo;",
  "ja354:9.3_0": "Evaṁsampadamevetaṁ,",
  "ja354:9.4_0": "yo petamanusocati.",
  "ja354:10.1_0": "Ḍayhamāno na jānāti,",
  "ja354:10.2_0": "ñātīnaṁ paridevitaṁ;",
  "ja354:10.3_0": "Tasmā etaṁ na socāmi,",
  "ja354:10.4_0": "gato so tassa yā gatī”ti.",
  "ja354:11.1_0": "Uragajātakaṁ catutthaṁ."
}