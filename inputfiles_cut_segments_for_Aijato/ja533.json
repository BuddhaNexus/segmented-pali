{
  "ja533:0.1_0": "Jātaka",
  "ja533:0.2_0": "Asītinipāta",
  "ja533:0.3_0": "Cūḷahaṁsavagga",
  "ja533:0.4_0": "1 Cūḷahaṁsajātaka",
  "ja533:1.1_0": "“Sumukha anupacinantā,",
  "ja533:1.2_0": "pakkamanti vihaṅgamā;",
  "ja533:1.3_0": "Gaccha tuvampi mā kaṅkhi,",
  "ja533:1.4_0": "natthi baddhe sahāyatā”.",
  "ja533:2.1_0": "“Gacche vāhaṁ na vā gacche,",
  "ja533:2.2_0": "na tena amaro siyaṁ;",
  "ja533:2.3_0": "Sukhitaṁ taṁ upāsitvā,",
  "ja533:2.4_0": "dukkhitaṁ taṁ kathaṁ jahe.",
  "ja533:3.1_0": "Maraṇaṁ vā tayā saddhiṁ,",
  "ja533:3.2_0": "jīvitaṁ vā tayā vinā;",
  "ja533:3.3_0": "Tadeva maraṇaṁ seyyo,",
  "ja533:3.4_0": "yañce jīve tayā vinā.",
  "ja533:4.1_0": "Nesa dhammo mahārāja,",
  "ja533:4.2_0": "yaṁ taṁ evaṁ gataṁ jahe;",
  "ja533:4.3_0": "Yā gati tuyhaṁ sā mayhaṁ,",
  "ja533:4.4_0": "ruccate vihagādhipa”.",
  "ja533:5.1_0": "“Kā nu pāsena baddhassa,",
  "ja533:5.2_0": "gati aññā mahānasā;",
  "ja533:5.3_0": "Sā kathaṁ cetayānassa,",
  "ja533:5.4_0": "muttassa tava ruccati.",
  "ja533:6.1_0": "Kaṁ vā tvaṁ passase atthaṁ,",
  "ja533:6.2_0": "mama tuyhañca pakkhima;",
  "ja533:6.3_0": "Ñātīnaṁ vāvasiṭṭhānaṁ,",
  "ja533:6.4_0": "ubhinnaṁ jīvitakkhaye.",
  "ja533:7.1_0": "Yaṁ na kañcanadepiñcha,",
  "ja533:7.2_0": "Andhena tamasā gataṁ;",
  "ja533:7.3_0": "Tādise sañcajaṁ pāṇaṁ,",
  "ja533:7.4_0": "Kamatthamabhijotaye”.",
  "ja533:8.1_0": "“Kathaṁ nu patataṁ seṭṭha,",
  "ja533:8.2_0": "Dhamme atthaṁ na bujjhasi;",
  "ja533:8.3_0": "Dhammo apacito santo,",
  "ja533:8.4_0": "Atthaṁ dasseti pāṇinaṁ.",
  "ja533:9.1_0": "Sohaṁ dhammaṁ apekkhāno,",
  "ja533:9.2_0": "dhammā catthaṁ samuṭṭhitaṁ;",
  "ja533:9.3_0": "Bhattiñca tayi sampassaṁ,",
  "ja533:9.4_0": "nāvakaṅkhāmi jīvitaṁ.",
  "ja533:10.1_0": "Addhā eso sataṁ dhammo,",
  "ja533:10.2_0": "yo mitto mittamāpade;",
  "ja533:10.3_0": "Na caje jīvitassāpi,",
  "ja533:10.4_0": "hetudhammamanussaraṁ”.",
  "ja533:11.1_0": "“Svāyaṁ dhammo ca te ciṇṇo,",
  "ja533:11.2_0": "bhatti ca viditā mayi;",
  "ja533:11.3_0": "Kāmaṁ karassu mayhetaṁ,",
  "ja533:11.4_0": "gacchevānumato mayā.",
  "ja533:12.1_0": "Api tvevaṁ gate kāle,",
  "ja533:12.2_0": "yaṁ khaṇḍaṁ ñātinaṁ mayā;",
  "ja533:12.3_0": "Tayā taṁ buddhisampannaṁ,",
  "ja533:12.4_0": "assa paramasaṁvutaṁ”.",
  "ja533:13.1_0": "Iccevaṁ mantayantānaṁ,",
  "ja533:13.2_0": "Ariyānaṁ ariyavuttinaṁ;",
  "ja533:13.3_0": "Paccadissatha nesādo,",
  "ja533:13.4_0": "Āturānamivantako.",
  "ja533:14.1_0": "Te sattumabhisañcikkha,",
  "ja533:14.2_0": "dīgharattaṁ hitā dijā;",
  "ja533:14.3_0": "Tuṇhīmāsittha ubhayo,",
  "ja533:14.4_0": "na sañcalesumāsanā.",
  "ja533:15.1_0": "Dhataraṭṭhe ca disvāna,",
  "ja533:15.2_0": "samuḍḍente tato tato;",
  "ja533:15.3_0": "Abhikkamatha vegena,",
  "ja533:15.4_0": "dijasattu dijādhipe.",
  "ja533:16.1_0": "So ca vegenabhikkamma,",
  "ja533:16.2_0": "āsajja parame dije;",
  "ja533:16.3_0": "Paccakamittha nesādo,",
  "ja533:16.4_0": "baddhā iti vicintayaṁ.",
  "ja533:17.1_0": "Ekaṁva baddhamāsīnaṁ,",
  "ja533:17.2_0": "abaddhañca punāparaṁ;",
  "ja533:17.3_0": "Āsajja baddhamāsīnaṁ,",
  "ja533:17.4_0": "pekkhamānamadīnavaṁ.",
  "ja533:18.1_0": "Tato so vimatoyeva,",
  "ja533:18.2_0": "paṇḍare ajjhabhāsatha;",
  "ja533:18.3_0": "Pavaḍḍhakāye āsīne,",
  "ja533:18.4_0": "dijasaṅghagaṇādhipe.",
  "ja533:19.1_0": "“Yannu pāsena mahatā,",
  "ja533:19.2_0": "baddho na kurute disaṁ;",
  "ja533:19.3_0": "Atha kasmā abaddho tvaṁ,",
  "ja533:19.4_0": "balī pakkhi na gacchasi.",
  "ja533:20.1_0": "Kiṁ nu tyāyaṁ dijo hoti,",
  "ja533:20.2_0": "mutto baddhaṁ upāsasi;",
  "ja533:20.3_0": "Ohāya sakuṇā yanti,",
  "ja533:20.4_0": "kiṁ eko avahīyasi”.",
  "ja533:21.1_0": "“Rājā me so dijāmitta,",
  "ja533:21.2_0": "sakhā pāṇasamo ca me;",
  "ja533:21.3_0": "Neva naṁ vijahissāmi,",
  "ja533:21.4_0": "yāva kālassa pariyāyaṁ”.",
  "ja533:22.1_0": "“Kathaṁ panāyaṁ vihaṅgo,",
  "ja533:22.2_0": "nāddasa pāsamoḍḍitaṁ;",
  "ja533:22.3_0": "Padañhetaṁ mahantānaṁ,",
  "ja533:22.4_0": "boddhumarahanti āpadaṁ”.",
  "ja533:23.1_0": "“Yadā parābhavo hoti,",
  "ja533:23.2_0": "poso jīvitasaṅkhaye;",
  "ja533:23.3_0": "Atha jālañca pāsañca,",
  "ja533:23.4_0": "āsajjāpi na bujjhati.",
  "ja533:24.1_0": "Api tveva mahāpañña,",
  "ja533:24.2_0": "pāsā bahuvidhā tatā;",
  "ja533:24.3_0": "Guyhamāsajja bajjhanti,",
  "ja533:24.4_0": "athevaṁ jīvitakkhaye”.",
  "ja533:25.1_0": "“Api nāyaṁ tayā saddhiṁ,",
  "ja533:25.2_0": "saṁvāsassa sukhudrayo;",
  "ja533:25.3_0": "Api no anumaññāsi,",
  "ja533:25.4_0": "api no jīvitaṁ dade”.",
  "ja533:26.1_0": "“Na ceva me tvaṁ baddhosi,",
  "ja533:26.2_0": "napi icchāmi te vadhaṁ;",
  "ja533:26.3_0": "Kāmaṁ khippamito gantvā,",
  "ja533:26.4_0": "jīva tvaṁ anigho ciraṁ”.",
  "ja533:27.1_0": "“Nevāhametamicchāmi,",
  "ja533:27.2_0": "aññatretassa jīvitā;",
  "ja533:27.3_0": "Sace ekena tuṭṭhosi,",
  "ja533:27.4_0": "muñcetaṁ mañca bhakkhaya.",
  "ja533:28.1_0": "Ārohapariṇāhena,",
  "ja533:28.2_0": "tulyāsmā vayasā ubho;",
  "ja533:28.3_0": "Na te lābhena jīvatthi,",
  "ja533:28.4_0": "etena niminā tuvaṁ.",
  "ja533:29.1_0": "Tadiṅgha samapekkhassu,",
  "ja533:29.2_0": "hotu giddhi tavamhasu;",
  "ja533:29.3_0": "Maṁ pubbe bandha pāsena,",
  "ja533:29.4_0": "pacchā muñca dijādhipaṁ.",
  "ja533:30.1_0": "Tāvadeva ca te lābho,",
  "ja533:30.2_0": "katāssa yācanāya ca;",
  "ja533:30.3_0": "Mitti ca dhataraṭṭhehi,",
  "ja533:30.4_0": "yāvajīvāya te siyā”.",
  "ja533:31.1_0": "“Passantu no mahāsaṅghā,",
  "ja533:31.2_0": "tayā muttaṁ ito gataṁ;",
  "ja533:31.3_0": "Mittāmaccā ca bhaccā ca,",
  "ja533:31.4_0": "puttadārā ca bandhavā.",
  "ja533:32.1_0": "Na ca te tādisā mittā,",
  "ja533:32.2_0": "bahūnaṁ idha vijjati;",
  "ja533:32.3_0": "Yathā tvaṁ dhataraṭṭhassa,",
  "ja533:32.4_0": "pāṇasādhāraṇo sakhā.",
  "ja533:33.1_0": "So te sahāyaṁ muñcāmi,",
  "ja533:33.2_0": "hotu rājā tavānugo;",
  "ja533:33.3_0": "Kāmaṁ khippamito gantvā,",
  "ja533:33.4_0": "ñātimajjhe virocatha”.",
  "ja533:34.1_0": "“So patīto pamuttena,",
  "ja533:34.2_0": "bhattunā bhattugāravo;",
  "ja533:34.3_0": "Ajjhabhāsatha vakkaṅgo,",
  "ja533:34.4_0": "vācaṁ kaṇṇasukhaṁ bhaṇaṁ.",
  "ja533:35.1_0": "Evaṁ luddaka nandassu,",
  "ja533:35.2_0": "saha sabbehi ñātibhi;",
  "ja533:35.3_0": "Yathāhamajja nandāmi,",
  "ja533:35.4_0": "muttaṁ disvā dijādhipaṁ”.",
  "ja533:36.1_0": "“Ehi taṁ anusikkhāmi,",
  "ja533:36.2_0": "Yathā tvamapi lacchase;",
  "ja533:36.3_0": "Lābhaṁ tavāyaṁ dhataraṭṭho,",
  "ja533:36.4_0": "Pāpaṁ kiñci na dakkhati.",
  "ja533:37.1_0": "Khippamantepuraṁ netvā,",
  "ja533:37.2_0": "rañño dassehi no ubho;",
  "ja533:37.3_0": "Abaddhe pakatibhūte,",
  "ja533:37.4_0": "kāje ubhayato ṭhite.",
  "ja533:38.1_0": "Dhataraṭṭhā mahārāja,",
  "ja533:38.2_0": "haṁsādhipatino ime;",
  "ja533:38.3_0": "Ayañhi rājā haṁsānaṁ,",
  "ja533:38.4_0": "ayaṁ senāpatītaro.",
  "ja533:39.1_0": "Asaṁsayaṁ imaṁ disvā,",
  "ja533:39.2_0": "haṁsarājaṁ narādhipo;",
  "ja533:39.3_0": "Patīto sumano vitto,",
  "ja533:39.4_0": "bahuṁ dassati te dhanaṁ”.",
  "ja533:40.1_0": "Tassa taṁ vacanaṁ sutvā,",
  "ja533:40.2_0": "kammunā upapādayi;",
  "ja533:40.3_0": "Khippamantepuraṁ gantvā,",
  "ja533:40.4_0": "rañño haṁse adassayi;",
  "ja533:40.5_0": "Abaddhe pakatibhūte,",
  "ja533:40.6_0": "kāje ubhayato ṭhite.",
  "ja533:41.1_0": "“Dhataraṭṭhā mahārāja,",
  "ja533:41.2_0": "haṁsādhipatino ime;",
  "ja533:41.3_0": "Ayañhi rājā haṁsānaṁ,",
  "ja533:41.4_0": "ayaṁ senāpatītaro”.",
  "ja533:42.1_0": "“Kathaṁ panime vihaṅgā,",
  "ja533:42.2_0": "tava hatthattamāgatā;",
  "ja533:42.3_0": "Kathaṁ luddo mahantānaṁ,",
  "ja533:42.4_0": "issare idha ajjhagā”.",
  "ja533:43.1_0": "“Vihitā santime pāsā,",
  "ja533:43.2_0": "pallalesu janādhipa;",
  "ja533:43.3_0": "Yaṁ yadāyatanaṁ maññe,",
  "ja533:43.4_0": "dijānaṁ pāṇarodhanaṁ.",
  "ja533:44.1_0": "Tādisaṁ pāsamāsajja,",
  "ja533:44.2_0": "haṁsarājā abajjhatha;",
  "ja533:44.3_0": "Taṁ abaddho upāsīno,",
  "ja533:44.4_0": "mamāyaṁ ajjhabhāsatha.",
  "ja533:45.1_0": "Sudukkaraṁ anariyehi,",
  "ja533:45.2_0": "dahate bhāvamuttamaṁ;",
  "ja533:45.3_0": "Bhatturatthe parakkanto,",
  "ja533:45.4_0": "dhammayutto vihaṅgamo.",
  "ja533:46.1_0": "Attanāyaṁ cajitvāna,",
  "ja533:46.2_0": "jīvitaṁ jīvitāraho;",
  "ja533:46.3_0": "Anutthunanto āsīno,",
  "ja533:46.4_0": "bhattu yācittha jīvitaṁ.",
  "ja533:47.1_0": "Tassa taṁ vacanaṁ sutvā,",
  "ja533:47.2_0": "pasādamahamajjhagā;",
  "ja533:47.3_0": "Tato naṁ pāmuciṁ pāsā,",
  "ja533:47.4_0": "anuññāsiṁ sukhena ca.",
  "ja533:48.1_0": "So patīto pamuttena,",
  "ja533:48.2_0": "bhattunā bhattugāravo;",
  "ja533:48.3_0": "Ajjhabhāsatha vakkaṅgo,",
  "ja533:48.4_0": "vācaṁ kaṇṇasukhaṁ bhaṇaṁ.",
  "ja533:49.1_0": "‘Evaṁ luddaka nandassu,",
  "ja533:49.2_0": "saha sabbehi ñātibhi;",
  "ja533:49.3_0": "Yathāhamajja nandāmi,",
  "ja533:49.4_0": "muttaṁ disvā dijādhipaṁ.",
  "ja533:50.1_0": "Ehi taṁ anusikkhāmi,",
  "ja533:50.2_0": "yathā tvamapi lacchase;",
  "ja533:50.3_0": "Lābhaṁ tavāyaṁ dhataraṭṭho,",
  "ja533:50.4_0": "pāpaṁ kiñci na dakkhati.",
  "ja533:51.1_0": "Khippamantepuraṁ netvā,",
  "ja533:51.2_0": "rañño dassehi no ubho;",
  "ja533:51.3_0": "Abaddhe pakatibhūte,",
  "ja533:51.4_0": "kāje ubhayato ṭhite.",
  "ja533:52.1_0": "“Dhataraṭṭhā mahārāja,",
  "ja533:52.2_0": "haṁsādhipatino ime;",
  "ja533:52.3_0": "Ayañhi rājā haṁsānaṁ,",
  "ja533:52.4_0": "ayaṁ senāpatītaro”.",
  "ja533:53.1_0": "Asaṁsayaṁ imaṁ disvā,",
  "ja533:53.2_0": "haṁsarājaṁ narādhipo;",
  "ja533:53.3_0": "Patīto sumano vitto,",
  "ja533:53.4_0": "bahuṁ dassati te dhanaṁ’.",
  "ja533:54.1_0": "Evametassa vacanā,",
  "ja533:54.2_0": "ānītāme ubho mayā;",
  "ja533:54.3_0": "Ettheva hi ime āsuṁ,",
  "ja533:54.4_0": "ubho anumatā mayā.",
  "ja533:55.1_0": "Soyaṁ evaṁ gato pakkhī,",
  "ja533:55.2_0": "dijo paramadhammiko;",
  "ja533:55.3_0": "Mādisassa hi luddassa,",
  "ja533:55.4_0": "janayeyyātha maddavaṁ.",
  "ja533:56.1_0": "Upāyanañca te deva,",
  "ja533:56.2_0": "nāññaṁ passāmi edisaṁ;",
  "ja533:56.3_0": "Sabbasākuṇikāgāme,",
  "ja533:56.4_0": "taṁ passa manujādhipa”.",
  "ja533:57.1_0": "Disvā nisinnaṁ rājānaṁ,",
  "ja533:57.2_0": "pīṭhe sovaṇṇaye subhe;",
  "ja533:57.3_0": "Ajjhabhāsatha vakkaṅgo,",
  "ja533:57.4_0": "vācaṁ kaṇṇasukhaṁ bhaṇaṁ.",
  "ja533:58.1_0": "“Kaccinnu bhoto kusalaṁ,",
  "ja533:58.2_0": "kacci bhoto anāmayaṁ;",
  "ja533:58.3_0": "Kacci raṭṭhamidaṁ phītaṁ,",
  "ja533:58.4_0": "dhammena manusāsasi”.",
  "ja533:59.1_0": "“Kusalañceva me haṁsa,",
  "ja533:59.2_0": "Atho haṁsa anāmayaṁ;",
  "ja533:59.3_0": "Atho raṭṭhamidaṁ phītaṁ,",
  "ja533:59.4_0": "Dhammena manusāsahaṁ”.",
  "ja533:60.1_0": "“Kacci bhoto amaccesu,",
  "ja533:60.2_0": "doso koci na vijjati;",
  "ja533:60.3_0": "Kacci ca te tavatthesu,",
  "ja533:60.4_0": "nāvakaṅkhanti jīvitaṁ”.",
  "ja533:61.1_0": "“Athopi me amaccesu,",
  "ja533:61.2_0": "doso koci na vijjati;",
  "ja533:61.3_0": "Athopi te mamatthesu,",
  "ja533:61.4_0": "nāvakaṅkhanti jīvitaṁ”.",
  "ja533:62.1_0": "“Kacci te sādisī bhariyā,",
  "ja533:62.2_0": "assavā piyabhāṇinī;",
  "ja533:62.3_0": "Puttarūpayasūpetā,",
  "ja533:62.4_0": "tava chandavasānugā”.",
  "ja533:63.1_0": "“Atho me sādisī bhariyā,",
  "ja533:63.2_0": "assavā piyabhāṇinī;",
  "ja533:63.3_0": "Puttarūpayasūpetā,",
  "ja533:63.4_0": "mama chandavasānugā.",
  "ja533:64.1_0": "Bhavantaṁ kacci nu mahā-",
  "ja533:64.2_0": "sattuhatthattataṁ gato;",
  "ja533:64.3_0": "Dukkhamāpajji vipulaṁ,",
  "ja533:64.4_0": "tasmiṁ paṭhamamāpade.",
  "ja533:65.1_0": "Kacci yantāpatitvāna,",
  "ja533:65.2_0": "daṇḍena samapothayi;",
  "ja533:65.3_0": "Evametesaṁ jammānaṁ,",
  "ja533:65.4_0": "pātikaṁ bhavati tāvade”.",
  "ja533:66.1_0": "“Khemamāsi mahārāja,",
  "ja533:66.2_0": "evamāpadiyā sati;",
  "ja533:66.3_0": "Na cāyaṁ kiñci rasmāsu,",
  "ja533:66.4_0": "sattūva samapajjatha.",
  "ja533:67.1_0": "Paccagamittha nesādo,",
  "ja533:67.2_0": "pubbeva ajjhabhāsatha;",
  "ja533:67.3_0": "Tadāyaṁ sumukhoyeva,",
  "ja533:67.4_0": "paṇḍito paccabhāsatha.",
  "ja533:68.1_0": "Tassa taṁ vacanaṁ sutvā,",
  "ja533:68.2_0": "pasādamayamajjhagā;",
  "ja533:68.3_0": "Tato maṁ pāmucī pāsā,",
  "ja533:68.4_0": "anuññāsi sukhena ca.",
  "ja533:69.1_0": "Idañca sumukheneva,",
  "ja533:69.2_0": "etadatthāya cintitaṁ;",
  "ja533:69.3_0": "Bhoto sakāsegamanaṁ,",
  "ja533:69.4_0": "etassa dhanamicchatā”.",
  "ja533:70.1_0": "“Svāgatañcevidaṁ bhavataṁ,",
  "ja533:70.2_0": "patīto casmi dassanā;",
  "ja533:70.3_0": "Eso cāpi bahuṁ vittaṁ,",
  "ja533:70.4_0": "labhataṁ yāvadicchati”.",
  "ja533:71.1_0": "Santappayitvā nesādaṁ,",
  "ja533:71.2_0": "bhogehi manujādhipo;",
  "ja533:71.3_0": "Ajjhabhāsatha vakkaṅgaṁ,",
  "ja533:71.4_0": "vācaṁ kaṇṇasukhaṁ bhaṇaṁ.",
  "ja533:72.1_0": "“Yaṁ khalu dhammamādhīnaṁ,",
  "ja533:72.2_0": "vaso vattati kiñcanaṁ;",
  "ja533:72.3_0": "Sabbatthissariyaṁ tava,",
  "ja533:72.4_0": "taṁ pasāsa yadicchatha.",
  "ja533:73.1_0": "Dānatthaṁ upabhottuṁ vā,",
  "ja533:73.2_0": "yaṁ caññaṁ upakappati;",
  "ja533:73.3_0": "Etaṁ dadāmi vo vittaṁ,",
  "ja533:73.4_0": "issariyaṁ vissajāmi vo”.",
  "ja533:74.1_0": "“Yathā ca myāyaṁ sumukho,",
  "ja533:74.2_0": "ajjhabhāseyya paṇḍito;",
  "ja533:74.3_0": "Kāmasā buddhisampanno,",
  "ja533:74.4_0": "taṁ myāssa paramappiyaṁ”.",
  "ja533:75.1_0": "“Ahaṁ khalu mahārāja,",
  "ja533:75.2_0": "nāgarājārivantaraṁ;",
  "ja533:75.3_0": "Paṭivattuṁ na sakkomi,",
  "ja533:75.4_0": "na me so vinayo siyā.",
  "ja533:76.1_0": "Amhākañceva so seṭṭho,",
  "ja533:76.2_0": "tvañca uttamasattavo;",
  "ja533:76.3_0": "Bhūmipālo manussindo,",
  "ja533:76.4_0": "pūjā bahūhi hetuhi.",
  "ja533:77.1_0": "Tesaṁ ubhinnaṁ bhaṇataṁ,",
  "ja533:77.2_0": "vattamāne vinicchaye;",
  "ja533:77.3_0": "Nantaraṁ paṭivattabbaṁ,",
  "ja533:77.4_0": "pessena manujādhipa”.",
  "ja533:78.1_0": "“Dhammena kira nesādo,",
  "ja533:78.2_0": "paṇḍito aṇḍajo iti;",
  "ja533:78.3_0": "Na heva akatattassa,",
  "ja533:78.4_0": "nayo etādiso siyā.",
  "ja533:79.1_0": "Evaṁ aggapakatimā,",
  "ja533:79.2_0": "evaṁ uttamasattavo;",
  "ja533:79.3_0": "Yāvatatthi mayā diṭṭhā,",
  "ja533:79.4_0": "nāññaṁ passāmi edisaṁ.",
  "ja533:80.1_0": "Tuṭṭhosmi vo pakatiyā,",
  "ja533:80.2_0": "vākyena madhurena ca;",
  "ja533:80.3_0": "Eso cāpi mamacchando,",
  "ja533:80.4_0": "ciraṁ passeyya vo ubho”.",
  "ja533:81.1_0": "“Yaṁ kiccaṁ parame mitte,",
  "ja533:81.2_0": "katamasmāsu taṁ tayā;",
  "ja533:81.3_0": "Pattā nissaṁsayaṁ tyāmhā,",
  "ja533:81.4_0": "bhattirasmāsu yā tava.",
  "ja533:82.1_0": "Aduñca nūna sumahā,",
  "ja533:82.2_0": "ñātisaṅghassa mantaraṁ;",
  "ja533:82.3_0": "Adassanena asmākaṁ,",
  "ja533:82.4_0": "dukkhaṁ bahūsu pakkhisu.",
  "ja533:83.1_0": "Tesaṁ sokavighātāya,",
  "ja533:83.2_0": "Tayā anumatā mayaṁ;",
  "ja533:83.3_0": "Taṁ padakkhiṇato katvā,",
  "ja533:83.4_0": "Ñātiṁ passemurindama.",
  "ja533:84.1_0": "Addhāhaṁ vipulaṁ pītiṁ,",
  "ja533:84.2_0": "bhavataṁ vindāmi dassanā;",
  "ja533:84.3_0": "Eso cāpi mahā attho,",
  "ja533:84.4_0": "ñātivissāsanā siyā”.",
  "ja533:85.1_0": "Idaṁ vatvā dhataraṭṭho,",
  "ja533:85.2_0": "haṁsarājā narādhipaṁ;",
  "ja533:85.3_0": "Uttamaṁ javamanvāya,",
  "ja533:85.4_0": "ñātisaṅghaṁ upāgamuṁ.",
  "ja533:86.1_0": "Te aroge anuppatte,",
  "ja533:86.2_0": "disvāna parame dije;",
  "ja533:86.3_0": "Kekāti makaruṁ haṁsā,",
  "ja533:86.4_0": "puthusaddo ajāyatha.",
  "ja533:87.1_0": "Te patītā pamuttena,",
  "ja533:87.2_0": "bhattunā bhattugāravā;",
  "ja533:87.3_0": "Samantā parikiriṁsu,",
  "ja533:87.4_0": "aṇḍajā laddhapaccayā.",
  "ja533:88.1_0": "Evaṁ mittavataṁ atthā,",
  "ja533:88.2_0": "sabbe honti padakkhiṇā;",
  "ja533:88.3_0": "Haṁsā yathā dhataraṭṭhā,",
  "ja533:88.4_0": "ñātisaṅghaṁ upāgamunti.",
  "ja533:89.1_0": "Cūḷahaṁsajātakaṁ paṭhamaṁ."
}