{
  "ja391:0.1_0": "Jātaka",
  "ja391:0.2_0": "Chakkanipāta",
  "ja391:0.3_0": "Kharaputtavagga",
  "ja391:0.4_0": "6. Vijjādharajātaka",
  "ja391:1.1_0": "“Dubbaṇṇarūpaṁ tuvamariyavaṇṇī,",
  "ja391:1.2_0": "Purakkhatvā pañjaliko namassasi;",
  "ja391:1.3_0": "Seyyo nu te so udavā sarikkho,",
  "ja391:1.4_0": "Nāmaṁ parassattano cāpi brūhi”.",
  "ja391:2.1_0": "“Na nāmagottaṁ gaṇhanti rāja,",
  "ja391:2.2_0": "Sammaggatānujjugatāna devā;",
  "ja391:2.3_0": "Ahañca te nāmadheyyaṁ vadāmi,",
  "ja391:2.4_0": "Sakkohamasmī tidasānamindo”.",
  "ja391:3.1_0": "“Yo disvā bhikkhuṁ caraṇūpapannaṁ,",
  "ja391:3.2_0": "Purakkhatvā pañjaliko namassati;",
  "ja391:3.3_0": "Pucchāmi taṁ devarājetamatthaṁ,",
  "ja391:3.4_0": "Ito cuto kiṁ labhate sukhaṁ so”.",
  "ja391:4.1_0": "“Yo disvā bhikkhuṁ caraṇūpapannaṁ,",
  "ja391:4.2_0": "Purakkhatvā pañjaliko namassati;",
  "ja391:4.3_0": "Diṭṭheva dhamme labhate pasaṁsaṁ,",
  "ja391:4.4_0": "Saggañca so yāti sarīrabhedā”.",
  "ja391:5.1_0": "“Lakkhī vata me udapādi ajja,",
  "ja391:5.2_0": "Yaṁ vāsavaṁ bhūtapatiddasāma;",
  "ja391:5.3_0": "Bhikkhuñca disvāna tuvañca sakka,",
  "ja391:5.4_0": "Kāhāmi puññāni anappakāni”.",
  "ja391:6.1_0": "“Addhā have sevitabbā sapaññā,",
  "ja391:6.2_0": "Bahussutā ye bahuṭhānacintino;",
  "ja391:6.3_0": "Bhikkhuñca disvāna mamañca rāja,",
  "ja391:6.4_0": "Karohi puññāni anappakāni”.",
  "ja391:7.1_0": "“Akkodhano niccapasannacitto,",
  "ja391:7.2_0": "Sabbātithīyācayogo bhavitvā;",
  "ja391:7.3_0": "Nihacca mānaṁ abhivādayissaṁ,",
  "ja391:7.4_0": "Sutvāna devinda subhāsitānī”ti.",
  "ja391:8.1_0": "Vijjādharajātakaṁ chaṭṭhaṁ."
}