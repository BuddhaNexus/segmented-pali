{
  "tha-ap236:0.1_0": "Therāpadāna",
  "tha-ap236:0.2_0": "Udakāsanavagga",
  "tha-ap236:0.3_0": "4. Kilañjadāyakattheraapadāna",
  "tha-ap236:1.1_0": "“Tivarāyaṁ pure ramme,",
  "tha-ap236:1.2_0": "naḷakāro ahaṁ tadā;",
  "tha-ap236:1.3_0": "Siddhatthe lokapajjote,",
  "tha-ap236:1.4_0": "pasannā janatā tahiṁ.",
  "tha-ap236:2.1_0": "Pūjatthaṁ lokanāthassa,",
  "tha-ap236:2.2_0": "kilañjaṁ pariyesati;",
  "tha-ap236:2.3_0": "Buddhapūjaṁ karontānaṁ,",
  "tha-ap236:2.4_0": "kilañjaṁ adadiṁ ahaṁ.",
  "tha-ap236:3.1_0": "Catunnavutito kappe,",
  "tha-ap236:3.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap236:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap236:3.4_0": "kilañjassa idaṁ phalaṁ.",
  "tha-ap236:4.1_0": "Sattasattatikappamhi,",
  "tha-ap236:4.2_0": "rājā āsiṁ jaladdharo;",
  "tha-ap236:4.3_0": "Sattaratanasampanno,",
  "tha-ap236:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap236:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap236:5.2_0": "…pe…",
  "tha-ap236:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap236:6.1_0": "Itthaṁ sudaṁ āyasmā kilañjadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap236:7.1_0": "Kilañjadāyakattherassāpadānaṁ catutthaṁ."
}