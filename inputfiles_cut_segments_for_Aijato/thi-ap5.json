{
  "thi-ap5:0.1_0": "Therīapadāna",
  "thi-ap5:0.2_0": "Sumedhāvagga",
  "thi-ap5:0.3_0": "5. Naḷamālikātherīapadāna",
  "thi-ap5:1.1_0": "“Candabhāgānadītīre,",
  "thi-ap5:1.2_0": "ahosiṁ kinnarī tadā;",
  "thi-ap5:1.3_0": "Addasaṁ virajaṁ buddhaṁ,",
  "thi-ap5:1.4_0": "sayambhuṁ aparājitaṁ.",
  "thi-ap5:2.1_0": "Pasannacittā sumanā,",
  "thi-ap5:2.2_0": "vedajātā katañjalī;",
  "thi-ap5:2.3_0": "Naḷamālaṁ gahetvāna,",
  "thi-ap5:2.4_0": "sayambhuṁ abhipūjayiṁ.",
  "thi-ap5:3.1_0": "Tena kammena sukatena,",
  "thi-ap5:3.2_0": "cetanāpaṇidhīhi ca;",
  "thi-ap5:3.3_0": "Jahitvā kinnarīdehaṁ,",
  "thi-ap5:3.4_0": "agacchiṁ tidasaṅgatiṁ.",
  "thi-ap5:4.1_0": "Chattiṁsadevarājūnaṁ,",
  "thi-ap5:4.2_0": "mahesittamakārayiṁ;",
  "thi-ap5:4.3_0": "Dasannaṁ cakkavattīnaṁ,",
  "thi-ap5:4.4_0": "mahesittamakārayiṁ;",
  "thi-ap5:4.5_0": "Saṁvejetvāna me cittaṁ,",
  "thi-ap5:4.6_0": "pabbajiṁ anagāriyaṁ.",
  "thi-ap5:5.1_0": "Kilesā jhāpitā mayhaṁ,",
  "thi-ap5:5.2_0": "bhavā sabbe samūhatā;",
  "thi-ap5:5.3_0": "Sabbāsavā parikkhīṇā,",
  "thi-ap5:5.4_0": "natthi dāni punabbhavo.",
  "thi-ap5:6.1_0": "Catunnavutito kappe,",
  "thi-ap5:6.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "thi-ap5:6.3_0": "Duggatiṁ nābhijānāmi,",
  "thi-ap5:6.4_0": "pupphapūjāyidaṁ phalaṁ.",
  "thi-ap5:7.1_0": "Kilesā jhāpitā mayhaṁ,",
  "thi-ap5:7.2_0": "…pe…",
  "thi-ap5:7.3_0": "viharāmi anāsavā.",
  "thi-ap5:8.1_0": "Svāgataṁ vata me āsi,",
  "thi-ap5:8.2_0": "…pe…",
  "thi-ap5:8.3_0": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap5:9.1_0": "Paṭisambhidā catasso,",
  "thi-ap5:9.2_0": "…pe…",
  "thi-ap5:9.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "thi-ap5:10.1_0": "Itthaṁ sudaṁ naḷamālikā therī imā gāthāyo abhāsitthāti.",
  "thi-ap5:11.1_0": "Naḷamālikātheriyāpadānaṁ pañcamaṁ."
}