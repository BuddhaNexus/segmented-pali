{
  "ja255:0.1_0": "Jātaka",
  "ja255:0.2_0": "Tikanipāta",
  "ja255:0.3_0": "Saṅkappavagga",
  "ja255:0.4_0": "5. Sukajātaka",
  "ja255:1.1_0": "“Yāva so mattamaññāsi,",
  "ja255:1.2_0": "bhojanasmiṁ vihaṅgamo;",
  "ja255:1.3_0": "Tāva addhānamāpādi,",
  "ja255:1.4_0": "mātarañca aposayi.",
  "ja255:2.1_0": "Yato ca kho bahutaraṁ,",
  "ja255:2.2_0": "bhojanaṁ ajjhavāhari;",
  "ja255:2.3_0": "Tato tattheva saṁsīdi,",
  "ja255:2.4_0": "amattaññū hi so ahu.",
  "ja255:3.1_0": "Tasmā mattaññutā sādhu,",
  "ja255:3.2_0": "bhojanasmiṁ agiddhatā;",
  "ja255:3.3_0": "Amattaññū hi sīdanti,",
  "ja255:3.4_0": "mattaññū ca na sīdare”ti.",
  "ja255:4.1_0": "Sukajātakaṁ pañcamaṁ."
}