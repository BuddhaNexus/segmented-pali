{
  "ja240:0.1_0": "Jātaka",
  "ja240:0.2_0": "Dukanipāta",
  "ja240:0.3_0": "Upāhanavagga",
  "ja240:0.4_0": "10. Mahāpiṅgalajātaka",
  "ja240:1.1_0": "“Sabbo jano hiṁsito piṅgalena,",
  "ja240:1.2_0": "Tasmiṁ mate paccayā vedayanti;",
  "ja240:1.3_0": "Piyo nu te āsi akaṇhanetto,",
  "ja240:1.4_0": "Kasmā nu tvaṁ rodasi dvārapāla”.",
  "ja240:2.1_0": "“Na me piyo āsi akaṇhanetto,",
  "ja240:2.2_0": "Bhāyāmi paccāgamanāya tassa;",
  "ja240:2.3_0": "Ito gato hiṁseyya maccurājaṁ,",
  "ja240:2.4_0": "So hiṁsito āneyya puna idha”.",
  "ja240:3.1_0": "“Daḍḍho vāhasahassehi,",
  "ja240:3.2_0": "sitto ghaṭasatehi so;",
  "ja240:3.3_0": "Parikkhatā ca sā bhūmi,",
  "ja240:3.4_0": "mā bhāyi nāgamissatī”ti.",
  "ja240:4.1_0": "Mahāpiṅgalajātakaṁ dasamaṁ.",
  "ja240:4.2_0": "Upāhanavaggo navamo.",
  "ja240:5.1_0": "Tassuddānaṁ",
  "ja240:6.1_0": "Varupāhana khujja vikaṇṇakako,",
  "ja240:6.2_0": "Asitābhuya pañcamavacchanakho;",
  "ja240:6.3_0": "Dija pemavaruttamaekapadaṁ,",
  "ja240:6.4_0": "Kumināmukha piṅgalakena dasāti."
}