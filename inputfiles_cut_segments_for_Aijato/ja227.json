{
  "ja227:0.1_0": "Jātaka",
  "ja227:0.2_0": "Dukanipāta",
  "ja227:0.3_0": "Kāsāvavagga",
  "ja227:0.4_0": "7. Gūthapāṇajātaka",
  "ja227:1.1_0": "“Sūro sūrena saṅgamma,",
  "ja227:1.2_0": "vikkantena pahārinā;",
  "ja227:1.3_0": "Ehi nāga nivattassu,",
  "ja227:1.4_0": "kiṁ nu bhīto palāyasi;",
  "ja227:1.5_0": "Passantu aṅgamagadhā,",
  "ja227:1.6_0": "mama tuyhañca vikkamaṁ”.",
  "ja227:2.1_0": "“Na taṁ pādā vadhissāmi,",
  "ja227:2.2_0": "na dantehi na soṇḍiyā;",
  "ja227:2.3_0": "Mīḷhena taṁ vadhissāmi,",
  "ja227:2.4_0": "pūti haññatu pūtinā”ti.",
  "ja227:3.1_0": "Gūthapāṇajātakaṁ sattamaṁ."
}