{
  "kv12.6:0.1_0": "Kathāvatthu",
  "kv12.6:0.2_0": "Mahāpaṇṇāsaka",
  "kv12.6:0.3_0": "Dvādasamavagga",
  "kv12.6:0.4_0": "Kolaṅkolakathā",
  "kv12.6:1.1_0": "Na vattabbaṁ—",
  "kv12.6:1.2_0": "“kolaṅkolo puggalo kolaṅkolatāniyato”ti?",
  "kv12.6:1.3_0": "Āmantā.",
  "kv12.6:1.4_0": "Nanu so kolaṅkoloti?",
  "kv12.6:1.5_0": "Āmantā.",
  "kv12.6:1.6_0": "Hañci so kolaṅkolo, tena vata re vattabbe—",
  "kv12.6:1.7_0": "“kolaṅkolo puggalo kolaṅkolatāniyato”ti.",
  "kv12.6:1.8_0": "Kolaṅkolakathā niṭṭhitā."
}