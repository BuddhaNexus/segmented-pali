{
  "thig6.8:0.1_0": "Therīgāthā",
  "thig6.8:0.2_0": "Chakkanipāta",
  "thig6.8:0.3_0": "8. Vijayātherīgāthā",
  "thig6.8:1.1_0": "“Catukkhattuṁ pañcakkhattuṁ,",
  "thig6.8:1.2_0": "vihārā upanikkhamiṁ;",
  "thig6.8:1.3_0": "Aladdhā cetaso santiṁ,",
  "thig6.8:1.4_0": "citte avasavattinī.",
  "thig6.8:2.1_0": "Bhikkhuniṁ upasaṅkamma,",
  "thig6.8:2.2_0": "sakkaccaṁ paripucchahaṁ;",
  "thig6.8:2.3_0": "Sā me dhammamadesesi,",
  "thig6.8:2.4_0": "dhātuāyatanāni ca.",
  "thig6.8:3.1_0": "Cattāri ariyasaccāni,",
  "thig6.8:3.2_0": "indriyāni balāni ca;",
  "thig6.8:3.3_0": "Bojjhaṅgaṭṭhaṅgikaṁ maggaṁ,",
  "thig6.8:3.4_0": "uttamatthassa pattiyā.",
  "thig6.8:4.1_0": "Tassāhaṁ vacanaṁ sutvā,",
  "thig6.8:4.2_0": "karontī anusāsaniṁ;",
  "thig6.8:4.3_0": "Rattiyā purime yāme,",
  "thig6.8:4.4_0": "pubbajātimanussariṁ.",
  "thig6.8:5.1_0": "Rattiyā majjhime yāme,",
  "thig6.8:5.2_0": "dibbacakkhuṁ visodhayiṁ;",
  "thig6.8:5.3_0": "Rattiyā pacchime yāme,",
  "thig6.8:5.4_0": "tamokhandhaṁ padālayiṁ.",
  "thig6.8:6.1_0": "Pītisukhena ca kāyaṁ,",
  "thig6.8:6.2_0": "Pharitvā vihariṁ tadā;",
  "thig6.8:6.3_0": "Sattamiyā pāde pasāresiṁ,",
  "thig6.8:6.4_0": "Tamokhandhaṁ padāliyā”ti.",
  "thig6.8:7.1_0": "…",
  "thig6.8:7.2_0": "Vijayā therī ….",
  "thig6.8:8.1_0": "Chakkanipāto niṭṭhito."
}