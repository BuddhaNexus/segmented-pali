{
  "vv52:0.1_0": "Vimānavatthu",
  "vv52:0.2_0": "Purisavimāna",
  "vv52:0.3_0": "Mahārathavagga",
  "vv52:0.4_0": "2. Revatīvimānavatthu",
  "vv52:1.1_0": "“Cirappavāsiṁ purisaṁ,",
  "vv52:1.2_0": "dūrato sotthimāgataṁ;",
  "vv52:1.3_0": "Ñātimittā suhajjā ca,",
  "vv52:1.4_0": "abhinandanti āgataṁ.",
  "vv52:2.1_0": "Tatheva katapuññampi,",
  "vv52:2.2_0": "asmā lokā paraṁ gataṁ;",
  "vv52:2.3_0": "Puññāni paṭigaṇhanti,",
  "vv52:2.4_0": "piyaṁ ñātīva āgataṁ”.",
  "vv52:3.1_0": "“Uṭṭhehi revate supāpadhamme,",
  "vv52:3.2_0": "Apārutadvāre adānasīle;",
  "vv52:3.3_0": "Nessāma taṁ yattha thunanti duggatā,",
  "vv52:3.4_0": "Samappitā nerayikā dukkhenā”ti.",
  "vv52:4.1_0": "Icceva vatvāna yamassa dūtā,",
  "vv52:4.2_0": "Te dve yakkhā lohitakkhā brahantā;",
  "vv52:4.3_0": "Paccekabāhāsu gahetvā revataṁ,",
  "vv52:4.4_0": "Pakkāmayuṁ devagaṇassa santike.",
  "vv52:5.1_0": "“Ādiccavaṇṇaṁ ruciraṁ pabhassaraṁ,",
  "vv52:5.2_0": "Byamhaṁ subhaṁ kañcanajālachannaṁ;",
  "vv52:5.3_0": "Kassetamākiṇṇajanaṁ vimānaṁ,",
  "vv52:5.4_0": "Suriyassa raṁsīriva jotamānaṁ.",
  "vv52:6.1_0": "Nārīgaṇā candanasāralittā,",
  "vv52:6.2_0": "Ubhato vimānaṁ upasobhayanti;",
  "vv52:6.3_0": "Taṁ dissati suriyasamānavaṇṇaṁ,",
  "vv52:6.4_0": "Ko modati saggapatto vimāne”ti.",
  "vv52:7.1_0": "“Bārāṇasiyaṁ nandiyo nāmāsi,",
  "vv52:7.2_0": "Upāsako amaccharī dānapati vadaññū;",
  "vv52:7.3_0": "Tassetamākiṇṇajanaṁ vimānaṁ,",
  "vv52:7.4_0": "Suriyassa raṁsīriva jotamānaṁ.",
  "vv52:8.1_0": "Nārīgaṇā candanasāralittā,",
  "vv52:8.2_0": "Ubhato vimānaṁ upasobhayanti;",
  "vv52:8.3_0": "Taṁ dissati suriyasamānavaṇṇaṁ,",
  "vv52:8.4_0": "So modati saggapatto vimāne”ti.",
  "vv52:9.1_0": "“Nandiyassāhaṁ bhariyā,",
  "vv52:9.2_0": "Agārinī sabbakulassa issarā;",
  "vv52:9.3_0": "Bhattu vimāne ramissāmi dānahaṁ,",
  "vv52:9.4_0": "Na patthaye nirayaṁ dassanāyā”ti.",
  "vv52:10.1_0": "“Eso te nirayo supāpadhamme,",
  "vv52:10.2_0": "Puññaṁ tayā akataṁ jīvaloke;",
  "vv52:10.3_0": "Na hi maccharī rosako pāpadhammo,",
  "vv52:10.4_0": "Saggūpagānaṁ labhati sahabyatan”ti.",
  "vv52:11.1_0": "“Kiṁ nu gūthañca muttañca,",
  "vv52:11.2_0": "asucī paṭidissati;",
  "vv52:11.3_0": "Duggandhaṁ kimidaṁ mīḷhaṁ,",
  "vv52:11.4_0": "kimetaṁ upavāyatī”ti.",
  "vv52:12.1_0": "“Esa saṁsavako nāma,",
  "vv52:12.2_0": "gambhīro sataporiso;",
  "vv52:12.3_0": "Yattha vassasahassāni,",
  "vv52:12.4_0": "tuvaṁ paccasi revate”ti.",
  "vv52:13.1_0": "“Kiṁ nu kāyena vācāya,",
  "vv52:13.2_0": "manasā dukkaṭaṁ kataṁ;",
  "vv52:13.3_0": "Kena saṁsavako laddho,",
  "vv52:13.4_0": "gambhīro sataporiso”ti.",
  "vv52:14.1_0": "“Samaṇe brāhmaṇe cāpi,",
  "vv52:14.2_0": "aññe vāpi vanibbake;",
  "vv52:14.3_0": "Musāvādena vañcesi,",
  "vv52:14.4_0": "taṁ pāpaṁ pakataṁ tayā.",
  "vv52:15.1_0": "Tena saṁsavako laddho,",
  "vv52:15.2_0": "gambhīro sataporiso;",
  "vv52:15.3_0": "Tattha vassasahassāni,",
  "vv52:15.4_0": "tuvaṁ paccasi revate.",
  "vv52:16.1_0": "Hatthepi chindanti athopi pāde,",
  "vv52:16.2_0": "Kaṇṇepi chindanti athopi nāsaṁ;",
  "vv52:16.3_0": "Athopi kākoḷagaṇā samecca,",
  "vv52:16.4_0": "Saṅgamma khādanti viphandamānan”ti.",
  "vv52:17.1_0": "“Sādhu kho maṁ paṭinetha,",
  "vv52:17.2_0": "kāhāmi kusalaṁ bahuṁ;",
  "vv52:17.3_0": "Dānena samacariyāya,",
  "vv52:17.4_0": "saṁyamena damena ca;",
  "vv52:17.5_0": "Yaṁ katvā sukhitā honti,",
  "vv52:17.6_0": "na ca pacchānutappare”ti.",
  "vv52:18.1_0": "“Pure tuvaṁ pamajjitvā,",
  "vv52:18.2_0": "idāni paridevasi;",
  "vv52:18.3_0": "Sayaṁ katānaṁ kammānaṁ,",
  "vv52:18.4_0": "vipākaṁ anubhossasī”ti.",
  "vv52:19.1_0": "“Ko devalokato manussalokaṁ,",
  "vv52:19.2_0": "Gantvāna puṭṭho me evaṁ vadeyya;",
  "vv52:19.3_0": "‘Nikkhittadaṇḍesu dadātha dānaṁ,",
  "vv52:19.4_0": "Acchādanaṁ seyyamathannapānaṁ;",
  "vv52:19.5_0": "Na hi maccharī rosako pāpadhammo,",
  "vv52:19.6_0": "Saggūpagānaṁ labhati sahabyataṁ’.",
  "vv52:20.1_0": "Sāhaṁ nūna ito gantvā,",
  "vv52:20.2_0": "yoniṁ laddhāna mānusiṁ;",
  "vv52:20.3_0": "Vadaññū sīlasampannā,",
  "vv52:20.4_0": "kāhāmi kusalaṁ bahuṁ;",
  "vv52:20.5_0": "Dānena samacariyāya,",
  "vv52:20.6_0": "saṁyamena damena ca.",
  "vv52:21.1_0": "Ārāmāni ca ropissaṁ,",
  "vv52:21.2_0": "dugge saṅkamanāni ca;",
  "vv52:21.3_0": "Papañca udapānañca,",
  "vv52:21.4_0": "vippasannena cetasā.",
  "vv52:22.1_0": "Cātuddasiṁ pañcadasiṁ,",
  "vv52:22.2_0": "yā ca pakkhassa aṭṭhamī;",
  "vv52:22.3_0": "Pāṭihāriyapakkhañca,",
  "vv52:22.4_0": "aṭṭhaṅgasusamāgataṁ.",
  "vv52:23.1_0": "Uposathaṁ upavasissaṁ,",
  "vv52:23.2_0": "sadā sīlesu saṁvutā;",
  "vv52:23.3_0": "Na ca dāne pamajjissaṁ,",
  "vv52:23.4_0": "sāmaṁ diṭṭhamidaṁ mayā”ti.",
  "vv52:24.1_0": "Iccevaṁ vippalapantiṁ,",
  "vv52:24.2_0": "phandamānaṁ tato tato;",
  "vv52:24.3_0": "Khipiṁsu niraye ghore,",
  "vv52:24.4_0": "uddhampādaṁ avaṁsiraṁ.",
  "vv52:25.1_0": "“Ahaṁ pure maccharinī ahosiṁ,",
  "vv52:25.2_0": "Paribhāsikā samaṇabrāhmaṇānaṁ;",
  "vv52:25.3_0": "Vitathena ca sāmikaṁ vañcayitvā,",
  "vv52:25.4_0": "Paccāmahaṁ niraye ghorarūpe”ti.",
  "vv52:26.1_0": "Revatīvimānaṁ dutiyaṁ."
}