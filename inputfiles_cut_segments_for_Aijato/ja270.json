{
  "ja270:0.1_0": "Jātaka",
  "ja270:0.2_0": "Tikanipāta",
  "ja270:0.3_0": "Padumavagga",
  "ja270:0.4_0": "10. Ulūkajātaka",
  "ja270:1.1_0": "“Sabbehi kira ñātīhi,",
  "ja270:1.2_0": "kosiyo issaro kato;",
  "ja270:1.3_0": "Sace ñātīhi anuññāto,",
  "ja270:1.4_0": "bhaṇeyyāhaṁ ekavācikaṁ”.",
  "ja270:2.1_0": "“Bhaṇa samma anuññāto,",
  "ja270:2.2_0": "atthaṁ dhammañca kevalaṁ;",
  "ja270:2.3_0": "Santi hi daharā pakkhī,",
  "ja270:2.4_0": "paññavanto jutindharā”.",
  "ja270:3.1_0": "“Na me ruccati bhaddaṁ vo,",
  "ja270:3.2_0": "ulūkassābhisecanaṁ;",
  "ja270:3.3_0": "Akkuddhassa mukhaṁ passa,",
  "ja270:3.4_0": "kathaṁ kuddho karissatī”ti.",
  "ja270:4.1_0": "Ulūkajātakaṁ dasamaṁ.",
  "ja270:4.2_0": "Padumavaggo dutiyo.",
  "ja270:5.1_0": "Tassuddānaṁ",
  "ja270:6.1_0": "Padumuttama nāgasirivhayano,",
  "ja270:6.2_0": "Sa-mahaṇṇava yūpa khurappavaro;",
  "ja270:6.3_0": "Atha bhaddalī kuñjara rukkha puna,",
  "ja270:6.4_0": "Kharavāca ulūkavarena dasāti."
}