{
  "iti86:0.1_0": "Itivuttaka 86",
  "iti86:0.2_0": "Tikanipāta",
  "iti86:0.3_0": "Catutthavagga",
  "iti86:0.4_0": "Dhammānudhammapaṭipannasutta",
  "iti86:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti86:2.1_0": "“Dhammānudhammapaṭipannassa bhikkhuno ayamanudhammo hoti veyyākaraṇāya—",
  "iti86:2.2_0": "dhammānudhammapaṭipannoyanti bhāsamāno dhammaññeva bhāsati no adhammaṁ, vitakkayamāno vā dhammavitakkaññeva vitakketi no adhammavitakkaṁ, tadubhayaṁ vā pana abhinivejjetvā upekkhako viharati sato sampajāno”ti.",
  "iti86:2.3_0": "Etamatthaṁ bhagavā avoca.",
  "iti86:2.4_0": "Tatthetaṁ iti vuccati:",
  "iti86:3.1_0": "“Dhammārāmo dhammarato,",
  "iti86:3.2_0": "dhammaṁ anuvicintayaṁ;",
  "iti86:3.3_0": "Dhammaṁ anussaraṁ bhikkhu,",
  "iti86:3.4_0": "saddhammā na parihāyati.",
  "iti86:4.1_0": "Caraṁ vā yadi vā tiṭṭhaṁ,",
  "iti86:4.2_0": "nisinno uda vā sayaṁ;",
  "iti86:4.3_0": "Ajjhattaṁ samayaṁ cittaṁ,",
  "iti86:4.4_0": "santimevādhigacchatī”ti.",
  "iti86:5.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti86:5.2_0": "Sattamaṁ."
}