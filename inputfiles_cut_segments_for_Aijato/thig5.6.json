{
  "thig5.6:0.1_0": "Therīgāthā",
  "thig5.6:0.2_0": "Pañcakanipāta",
  "thig5.6:0.3_0": "6 Mittākāḷītherīgāthā",
  "thig5.6:1.1_0": "“Saddhāya pabbajitvāna,",
  "thig5.6:1.2_0": "agārasmānagāriyaṁ;",
  "thig5.6:1.3_0": "Vicariṁhaṁ tena tena,",
  "thig5.6:1.4_0": "lābhasakkāraussukā.",
  "thig5.6:2.1_0": "Riñcitvā paramaṁ atthaṁ,",
  "thig5.6:2.2_0": "hīnamatthaṁ asevihaṁ;",
  "thig5.6:2.3_0": "Kilesānaṁ vasaṁ gantvā,",
  "thig5.6:2.4_0": "sāmaññatthaṁ na bujjhihaṁ.",
  "thig5.6:3.1_0": "Tassā me ahu saṁvego,",
  "thig5.6:3.2_0": "nisinnāya vihārake;",
  "thig5.6:3.3_0": "Ummaggapaṭipannāmhi,",
  "thig5.6:3.4_0": "taṇhāya vasamāgatā.",
  "thig5.6:4.1_0": "Appakaṁ jīvitaṁ mayhaṁ,",
  "thig5.6:4.2_0": "jarā byādhi ca maddati;",
  "thig5.6:4.3_0": "Purāyaṁ bhijjati kāyo,",
  "thig5.6:4.4_0": "na me kālo pamajjituṁ.",
  "thig5.6:5.1_0": "Yathābhūtamavekkhantī,",
  "thig5.6:5.2_0": "khandhānaṁ udayabbayaṁ;",
  "thig5.6:5.3_0": "Vimuttacittā uṭṭhāsiṁ,",
  "thig5.6:5.4_0": "kataṁ buddhassa sāsanan”ti.",
  "thig5.6:6.1_0": "… Mittā kāḷī therī …."
}