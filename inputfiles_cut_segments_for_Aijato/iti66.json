{
  "iti66:0.1_0": "Itivuttaka 66",
  "iti66:0.2_0": "Tikanipāta",
  "iti66:0.3_0": "Dutiyavagga",
  "iti66:0.4_0": "Soceyyasutta",
  "iti66:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti66:2.1_0": "“Tīṇimāni, bhikkhave, soceyyāni.",
  "iti66:2.2_0": "Katamāni tīṇi?",
  "iti66:2.3_0": "Kāyasoceyyaṁ, vacīsoceyyaṁ, manosoceyyaṁ—",
  "iti66:2.4_0": "imāni kho, bhikkhave, tīṇi soceyyānī”ti.",
  "iti66:2.5_0": "Etamatthaṁ bhagavā avoca.",
  "iti66:2.6_0": "Tatthetaṁ iti vuccati:",
  "iti66:3.1_0": "“Kāyasuciṁ vacīsuciṁ,",
  "iti66:3.2_0": "cetosucimanāsavaṁ;",
  "iti66:3.3_0": "Suciṁ soceyyasampannaṁ,",
  "iti66:3.4_0": "āhu sabbappahāyinan”ti.",
  "iti66:4.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti66:4.2_0": "Sattamaṁ."
}