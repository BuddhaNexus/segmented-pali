{
  "ja1:0.1_0": "Jātaka",
  "ja1:0.2_0": "Ekakanipāta",
  "ja1:0.3_0": "Apaṇṇakavagga",
  "ja1:0.4_0": "1. Apaṇṇakajātaka",
  "ja1:1.1_0": "Namo tassa Bhagavato Arahato Sammāsambuddhassa.",
  "ja1:2.1_0": "“Apaṇṇakaṁ ṭhānameke,",
  "ja1:2.2_0": "Dutiyaṁ āhu takkikā;",
  "ja1:2.3_0": "Etadaññāya medhāvī,",
  "ja1:2.4_0": "Taṁ gaṇhe yadapaṇṇakan”ti.",
  "ja1:3.1_0": "Apaṇṇakajātakaṁ paṭhamaṁ."
}