{
  "cp26:0.1_0": "Cariyāpiṭaka",
  "cp26:0.2_0": "Yudhañjayavagga",
  "cp26:0.3_0": "Adiṭṭhānapāramī 1",
  "cp26:0.4_0": "Temiyacariya",
  "cp26:1.1_0": "“Punāparaṁ yadā homi,",
  "cp26:1.2_0": "kāsirājassa atrajo;",
  "cp26:1.3_0": "Mūgapakkhoti nāmena,",
  "cp26:1.4_0": "temiyoti vadanti maṁ.",
  "cp26:2.1_0": "Soḷasitthisahassānaṁ,",
  "cp26:2.2_0": "na vijjati pumo tadā;",
  "cp26:2.3_0": "Ahorattānaṁ accayena,",
  "cp26:2.4_0": "nibbatto ahamekako.",
  "cp26:3.1_0": "Kicchā laddhaṁ piyaṁ puttaṁ,",
  "cp26:3.2_0": "abhijātaṁ jutindharaṁ;",
  "cp26:3.3_0": "Setacchattaṁ dhārayitvāna,",
  "cp26:3.4_0": "sayane poseti maṁ pitā.",
  "cp26:4.1_0": "Niddāyamāno sayanavare,",
  "cp26:4.2_0": "Pabujjhitvānahaṁ tadā;",
  "cp26:4.3_0": "Addasaṁ paṇḍaraṁ chattaṁ,",
  "cp26:4.4_0": "Yenāhaṁ nirayaṁ gato.",
  "cp26:5.1_0": "Saha diṭṭhassa me chattaṁ,",
  "cp26:5.2_0": "tāso uppajji bheravo;",
  "cp26:5.3_0": "Vinicchayaṁ samāpanno,",
  "cp26:5.4_0": "‘kathāhaṁ imaṁ muñcissaṁ’.",
  "cp26:6.1_0": "Pubbasālohitā mayhaṁ,",
  "cp26:6.2_0": "devatā atthakāminī;",
  "cp26:6.3_0": "Sā maṁ disvāna dukkhitaṁ,",
  "cp26:6.4_0": "tīsu ṭhānesu yojayi.",
  "cp26:7.1_0": "‘Mā paṇḍiccayaṁ vibhāvaya,",
  "cp26:7.2_0": "Bālamato bhava sabbapāṇinaṁ;",
  "cp26:7.3_0": "Sabbo taṁ jano ocināyatu,",
  "cp26:7.4_0": "Evaṁ tava attho bhavissati’.",
  "cp26:8.1_0": "Evaṁ vuttāyahaṁ tassā,",
  "cp26:8.2_0": "idaṁ vacanamabraviṁ;",
  "cp26:8.3_0": "‘Karomi te taṁ vacanaṁ,",
  "cp26:8.4_0": "yaṁ tvaṁ bhaṇasi devate;",
  "cp26:8.5_0": "Atthakāmāsi me amma,",
  "cp26:8.6_0": "hitakāmāsi devate’.",
  "cp26:9.1_0": "Tassāhaṁ vacanaṁ sutvā,",
  "cp26:9.2_0": "sāgareva thalaṁ labhiṁ;",
  "cp26:9.3_0": "Haṭṭho saṁviggamānaso,",
  "cp26:9.4_0": "tayo aṅge adhiṭṭhahiṁ.",
  "cp26:10.1_0": "Mūgo ahosiṁ badhiro,",
  "cp26:10.2_0": "pakkho gativivajjito;",
  "cp26:10.3_0": "Ete aṅge adhiṭṭhāya,",
  "cp26:10.4_0": "vassāni soḷasaṁ vasiṁ.",
  "cp26:11.1_0": "Tato me hatthapāde ca,",
  "cp26:11.2_0": "jivhaṁ sotañca maddiya;",
  "cp26:11.3_0": "Anūnataṁ me passitvā,",
  "cp26:11.4_0": "‘kāḷakaṇṇī’ti nindisuṁ.",
  "cp26:12.1_0": "Tato jānapadā sabbe,",
  "cp26:12.2_0": "senāpatipurohitā;",
  "cp26:12.3_0": "Sabbe ekamanā hutvā,",
  "cp26:12.4_0": "chaḍḍanaṁ anumodisuṁ.",
  "cp26:13.1_0": "Sohaṁ tesaṁ matiṁ sutvā,",
  "cp26:13.2_0": "haṭṭho saṁviggamānaso;",
  "cp26:13.3_0": "‘Yassatthāya tapociṇṇo,",
  "cp26:13.4_0": "so me attho samijjhatha’.",
  "cp26:14.1_0": "Nhāpetvā anulimpitvā,",
  "cp26:14.2_0": "veṭhetvā rājaveṭhanaṁ;",
  "cp26:14.3_0": "Chattena abhisiñcitvā,",
  "cp26:14.4_0": "kāresuṁ puraṁ padakkhiṇaṁ.",
  "cp26:15.1_0": "Sattāhaṁ dhārayitvāna,",
  "cp26:15.2_0": "uggate ravimaṇḍale;",
  "cp26:15.3_0": "Rathena maṁ nīharitvā,",
  "cp26:15.4_0": "sārathī vanamupāgami.",
  "cp26:16.1_0": "Ekokāse rathaṁ katvā,",
  "cp26:16.2_0": "sajjassaṁ hatthamuccito;",
  "cp26:16.3_0": "Sārathī khaṇatī kāsuṁ,",
  "cp26:16.4_0": "nikhātuṁ pathaviyā mamaṁ.",
  "cp26:17.1_0": "Adhiṭṭhitamadhiṭṭhānaṁ,",
  "cp26:17.2_0": "tajjento vividhakāraṇā;",
  "cp26:17.3_0": "Na bhindiṁ tamadhiṭṭhānaṁ,",
  "cp26:17.4_0": "bodhiyāyeva kāraṇā.",
  "cp26:18.1_0": "Mātāpitā na me dessā,",
  "cp26:18.2_0": "attā me na ca dessiyo;",
  "cp26:18.3_0": "Sabbaññutaṁ piyaṁ mayhaṁ,",
  "cp26:18.4_0": "tasmā vatamadhiṭṭhahiṁ.",
  "cp26:19.1_0": "Ete aṅge adhiṭṭhāya,",
  "cp26:19.2_0": "Vassāni soḷasaṁ vasiṁ;",
  "cp26:19.3_0": "Adhiṭṭhānena me samo natthi,",
  "cp26:19.4_0": "Esā me adhiṭṭhānapāramī”ti.",
  "cp26:20.1_0": "Temiyacariyaṁ chaṭṭhaṁ."
}