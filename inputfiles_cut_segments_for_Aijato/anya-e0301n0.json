{
  "anya-e0301n0:0_0": " Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e0301n0:1_0": "Paramatthadīpanī",
  "anya-e0301n0:2_0": "Saṅgaha mahāṭīkā pāṭha",
  "anya-e0301n0:3_0": "Ganthārabbhakathā",
  "anya-e0301n0:4_0": "[Ka]",
  "anya-e0301n0:5_0": "Udayā yassa ekassa, saddhammaraṁsi jālino;",
  "anya-e0301n0:6_0": "Pabujjhiṁsu janambujā, jātikkhette mahāsare.",
  "anya-e0301n0:7_0": "[Kha]",
  "anya-e0301n0:8_0": "Vandāmi taṁ mahāsūraṁ, mahāmoha tamonudaṁ;",
  "anya-e0301n0:9_0": "Sañjātaṁ so hadaye mayhaṁ, tamokhandhaṁ panūdataṁ.",
  "anya-e0301n0:10_0": "[Ga]",
  "anya-e0301n0:11_0": "Porāṇakehi viññūhi, vaṇṇitā vaṇṇanā bahū;",
  "anya-e0301n0:12_0": "Dissanti idha lokamhi, abhidhammatthasaṅgahe.",
  "anya-e0301n0:13_0": "[Gha]",
  "anya-e0301n0:14_0": "Na tāhi tuṭṭhiṁ vindanti, ye sāratthā bhimānino;",
  "anya-e0301n0:15_0": "Te maṁ saṅgamma yācanti, paramatthassa dīpanaṁ.",
  "anya-e0301n0:16_0": "[Ṅa]",
  "anya-e0301n0:17_0": "Mahaṇṇave ratanāni, uddharitvā dipavāsinaṁ;",
  "anya-e0301n0:18_0": "Yathicchakaṁpi dajjeyyuṁ, na vattabbāva ūnatā.",
  "anya-e0301n0:19_0": "[Ca]",
  "anya-e0301n0:20_0": "Tathevettha vipulatthā, aṇṇave ratanūpamā;",
  "anya-e0301n0:21_0": "Satakkhattuṁpi vaṇṇeyyuṁ, pariyādinnā na hessare.",
  "anya-e0301n0:22_0": "[Cha]",
  "anya-e0301n0:23_0": "Tasmā tāsu vaṇṇanāsu, sāramādāya vaṇṇanaṁ;",
  "anya-e0301n0:24_0": "Nānāsārattha sampuṇṇaṁ, uttāna padabyañjanaṁ.",
  "anya-e0301n0:25_0": "[Ja]",
  "anya-e0301n0:26_0": "Nātisaṅkhepa vitthāraṁ, mandabuddhippabodhanaṁ;",
  "anya-e0301n0:27_0": "Karissaṁ taṁ paramatthesu, suṇantu pāṭavatthinoti."
}