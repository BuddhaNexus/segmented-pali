{
  "ja483:0.1_0": "Jātaka",
  "ja483:0.2_0": "Terasakanipāta",
  "ja483:0.3_0": "Ambavagga",
  "ja483:0.4_0": "10. Sarabhamigajātaka",
  "ja483:1.1_0": "“Āsīsetheva puriso,",
  "ja483:1.2_0": "na nibbindeyya paṇḍito;",
  "ja483:1.3_0": "Passāmi vohaṁ attānaṁ,",
  "ja483:1.4_0": "yathā icchiṁ tathā ahu.",
  "ja483:2.1_0": "Āsīsetheva puriso,",
  "ja483:2.2_0": "na nibbindeyya paṇḍito;",
  "ja483:2.3_0": "Passāmi vohaṁ attānaṁ,",
  "ja483:2.4_0": "udakā thalamubbhataṁ.",
  "ja483:3.1_0": "Vāyametheva puriso,",
  "ja483:3.2_0": "na nibbindeyya paṇḍito;",
  "ja483:3.3_0": "Passāmi vohaṁ attānaṁ,",
  "ja483:3.4_0": "yathā icchiṁ tathā ahu.",
  "ja483:4.1_0": "Vāyametheva puriso,",
  "ja483:4.2_0": "na nibbindeyya paṇḍito;",
  "ja483:4.3_0": "Passāmi vohaṁ attānaṁ,",
  "ja483:4.4_0": "udakā thalamubbhataṁ.",
  "ja483:5.1_0": "Dukkhūpanītopi naro sapañño,",
  "ja483:5.2_0": "Āsaṁ na chindeyya sukhāgamāya;",
  "ja483:5.3_0": "Bahū hi phassā ahitā hitā ca,",
  "ja483:5.4_0": "Avitakkitā maccamupabbajanti.",
  "ja483:6.1_0": "Acintitampi bhavati,",
  "ja483:6.2_0": "cintitampi vinassati;",
  "ja483:6.3_0": "Na hi cintāmayā bhogā,",
  "ja483:6.4_0": "itthiyā purisassa vā”.",
  "ja483:7.1_0": "“Sarabhaṁ giriduggasmiṁ,",
  "ja483:7.2_0": "yaṁ tvaṁ anusarī pure;",
  "ja483:7.3_0": "Alīnacittassa tuvaṁ,",
  "ja483:7.4_0": "vikkantamanujīvasi.",
  "ja483:8.1_0": "Yo taṁ viduggā narakā samuddhari,",
  "ja483:8.2_0": "Silāya yoggaṁ sarabho karitvā;",
  "ja483:8.3_0": "Dukkhūpanītaṁ maccumukhā pamocayi,",
  "ja483:8.4_0": "Alīnacittaṁ ta migaṁ vadesi”.",
  "ja483:9.1_0": "“Kiṁ tvaṁ nu tattheva tadā ahosi,",
  "ja483:9.2_0": "Udāhu te koci naṁ etadakkhā;",
  "ja483:9.3_0": "Vivaṭṭacchaddo nusi sabbadassī,",
  "ja483:9.4_0": "Ñāṇaṁ nu te brāhmaṇa bhiṁsarūpaṁ”.",
  "ja483:10.1_0": "“Na cevahaṁ tattha tadā ahosiṁ,",
  "ja483:10.2_0": "Na cāpi me koci naṁ etadakkhā;",
  "ja483:10.3_0": "Gāthāpadānañca subhāsitānaṁ,",
  "ja483:10.4_0": "Atthaṁ tadānenti janinda dhīrā”.",
  "ja483:11.1_0": "“Ādāya pattiṁ paraviriyaghātiṁ,",
  "ja483:11.2_0": "Cāpe saraṁ kiṁ vicikicchase tuvaṁ;",
  "ja483:11.3_0": "Nunno saro sarabhaṁ hantu khippaṁ,",
  "ja483:11.4_0": "Annañhi etaṁ varapañña rañño”.",
  "ja483:12.1_0": "“Addhā pajānāmi ahampi etaṁ,",
  "ja483:12.2_0": "Annaṁ migo brāhmaṇa khattiyassa;",
  "ja483:12.3_0": "Pubbe katañca apacāyamāno,",
  "ja483:12.4_0": "Tasmā migaṁ sarabhaṁ no hanāmi”.",
  "ja483:13.1_0": "“Neso migo mahārāja,",
  "ja483:13.2_0": "asureso disampati;",
  "ja483:13.3_0": "Etaṁ hantvā manussinda,",
  "ja483:13.4_0": "bhavassu amarādhipo.",
  "ja483:14.1_0": "Sace ca rājā vicikicchase tuvaṁ,",
  "ja483:14.2_0": "Hantuṁ migaṁ sarabhaṁ sahāyakaṁ;",
  "ja483:14.3_0": "Saputtadāro naravīraseṭṭha,",
  "ja483:14.4_0": "Gantā tuvaṁ vetaraṇiṁ yamassa”.",
  "ja483:15.1_0": "“Kāmaṁ ahaṁ jānapadā ca sabbe,",
  "ja483:15.2_0": "Puttā ca dārā ca sahāyasaṅghā;",
  "ja483:15.3_0": "Gacchemu taṁ vetaraṇiṁ yamassa,",
  "ja483:15.4_0": "Na tveva hañño mama pāṇado yo.",
  "ja483:16.1_0": "Ayaṁ migo kicchagatassa mayhaṁ,",
  "ja483:16.2_0": "Ekassa kattā vivanasmi ghore;",
  "ja483:16.3_0": "Taṁ tādisaṁ pubbakiccaṁ saranto,",
  "ja483:16.4_0": "Jānaṁ mahābrahme kathaṁ haneyyaṁ”.",
  "ja483:17.1_0": "“Mittābhirādhī cirameva jīva,",
  "ja483:17.2_0": "Rajjaṁ imaṁ dhammaguṇe pasāsa;",
  "ja483:17.3_0": "Nārīgaṇehi paricāriyanto,",
  "ja483:17.4_0": "Modassu raṭṭhe tidiveva vāsavo.",
  "ja483:18.1_0": "Akkodhano niccapasannacitto,",
  "ja483:18.2_0": "Sabbātithī yācayogo bhavitvā;",
  "ja483:18.3_0": "Datvā ca bhutvā ca yathānubhāvaṁ,",
  "ja483:18.4_0": "Anindito saggamupehi ṭhānan”ti. ",
  "ja483:19.1_0": "Sarabhamigajātakaṁ dasamaṁ.",
  "ja483:20.1_0": "Terasakanipātaṁ niṭṭhitaṁ.",
  "ja483:21.1_0": "Tassuddānaṁ",
  "ja483:22.1_0": "Varaamba kuṭhāri sahaṁsavaro,",
  "ja483:22.2_0": "Atharaññasmiṁ dūtakapañcamako;",
  "ja483:22.3_0": "Atha bodhi akitti sutakkarinā,",
  "ja483:22.4_0": "Atha rurumigenaparo sarabhoti."
}