{
  "tha-ap302:0.1_0": "Therāpadāna",
  "tha-ap302:0.2_0": "Citakapūjakavagga",
  "tha-ap302:0.3_0": "10. Anusaṁsāvakattheraapadāna",
  "tha-ap302:1.1_0": "“Piṇḍāya caramānāhaṁ,",
  "tha-ap302:1.2_0": "vipassimaddasaṁ jinaṁ;",
  "tha-ap302:1.3_0": "Uḷuṅgabhikkhaṁ pādāsiṁ,",
  "tha-ap302:1.4_0": "dvipadindassa tādino.",
  "tha-ap302:2.1_0": "Pasannacitto sumano,",
  "tha-ap302:2.2_0": "abhivādesahaṁ tadā;",
  "tha-ap302:2.3_0": "Anusaṁsāvayiṁ buddhaṁ,",
  "tha-ap302:2.4_0": "uttamatthassa pattiyā.",
  "tha-ap302:3.1_0": "Ekanavutito kappe,",
  "tha-ap302:3.2_0": "anusaṁsāvayiṁ ahaṁ;",
  "tha-ap302:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap302:3.4_0": "anusaṁsāvanā phalaṁ.",
  "tha-ap302:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap302:4.2_0": "…pe…",
  "tha-ap302:4.3_0": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap302:5.1_0": "Itthaṁ sudaṁ āyasmā anusaṁsāvako thero imā gāthāyo abhāsitthāti.",
  "tha-ap302:6.1_0": "Anusaṁsāvakattherassāpadānaṁ dasamaṁ.",
  "tha-ap302:6.2_0": "Citakapūjakavaggo tiṁsatimo.",
  "tha-ap302:7.1_0": "Tassuddānaṁ",
  "tha-ap302:8.1_0": "Citakaṁ pārichatto ca,",
  "tha-ap302:8.2_0": "saddagosīsasantharaṁ;",
  "tha-ap302:8.3_0": "Pādo padesaṁ saraṇaṁ,",
  "tha-ap302:8.4_0": "ambo saṁsāvakopi ca;",
  "tha-ap302:8.5_0": "Aṭṭhatālīsa gāthāyo,",
  "tha-ap302:8.6_0": "gaṇitāyo vibhāvibhi.",
  "tha-ap302:9.1_0": "Atha vagguddānaṁ",
  "tha-ap302:10.1_0": "Kaṇikāro hatthidado,",
  "tha-ap302:10.2_0": "ālambaṇudakāsanaṁ;",
  "tha-ap302:10.3_0": "Tuvaraṁ thomako ceva,",
  "tha-ap302:10.4_0": "ukkhepaṁ sīsupadhānaṁ.",
  "tha-ap302:11.1_0": "Paṇṇado citapūjī ca,",
  "tha-ap302:11.2_0": "gāthāyo ceva sabbaso;",
  "tha-ap302:11.3_0": "Cattāri ca satānīha,",
  "tha-ap302:11.4_0": "ekapaññāsameva ca.",
  "tha-ap302:12.1_0": "Pañcavīsasatā sabbā,",
  "tha-ap302:12.2_0": "dvāsattati taduttari;",
  "tha-ap302:12.3_0": "Tisataṁ apadānānaṁ,",
  "tha-ap302:12.4_0": "gaṇitā atthadassibhi.",
  "tha-ap302:13.1_0": "Kaṇikāravaggadasakaṁ.",
  "tha-ap302:14.1_0": "Tatiyasatakaṁ samattaṁ."
}