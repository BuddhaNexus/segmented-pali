{
  "tha-ap139:0.1_0": "Therāpadāna",
  "tha-ap139:0.2_0": "Sobhitavagga",
  "tha-ap139:0.3_0": "7. Atthasandassakattheraapadāna",
  "tha-ap139:1.1_0": "“Visālamāḷe āsīno,",
  "tha-ap139:1.2_0": "addasaṁ lokanāyakaṁ;",
  "tha-ap139:1.3_0": "Khīṇāsavaṁ balappattaṁ,",
  "tha-ap139:1.4_0": "bhikkhusaṅghapurakkhataṁ.",
  "tha-ap139:2.1_0": "Satasahassā tevijjā,",
  "tha-ap139:2.2_0": "chaḷabhiññā mahiddhikā;",
  "tha-ap139:2.3_0": "Parivārenti sambuddhaṁ,",
  "tha-ap139:2.4_0": "ko disvā nappasīdati.",
  "tha-ap139:3.1_0": "Ñāṇe upanidhā yassa,",
  "tha-ap139:3.2_0": "na vijjati sadevake;",
  "tha-ap139:3.3_0": "Anantañāṇaṁ sambuddhaṁ,",
  "tha-ap139:3.4_0": "ko disvā nappasīdati.",
  "tha-ap139:4.1_0": "Dhammakāyañca dīpentaṁ,",
  "tha-ap139:4.2_0": "kevalaṁ ratanākaraṁ;",
  "tha-ap139:4.3_0": "Vikappetuṁ na sakkonti,",
  "tha-ap139:4.4_0": "ko disvā nappasīdati.",
  "tha-ap139:5.1_0": "Imāhi tīhi gāthāhi,",
  "tha-ap139:5.2_0": "nāradovhayavacchalo;",
  "tha-ap139:5.3_0": "Padumuttaraṁ thavitvāna,",
  "tha-ap139:5.4_0": "sambuddhaṁ aparājitaṁ.",
  "tha-ap139:6.1_0": "Tena cittappasādena,",
  "tha-ap139:6.2_0": "buddhasanthavanena ca;",
  "tha-ap139:6.3_0": "Kappānaṁ satasahassaṁ,",
  "tha-ap139:6.4_0": "duggatiṁ nupapajjahaṁ.",
  "tha-ap139:7.1_0": "Ito tiṁsakappasate,",
  "tha-ap139:7.2_0": "sumitto nāma khattiyo;",
  "tha-ap139:7.3_0": "Sattaratanasampanno,",
  "tha-ap139:7.4_0": "cakkavattī mahabbalo.",
  "tha-ap139:8.1_0": "Paṭisambhidā catasso,",
  "tha-ap139:8.2_0": "…pe…",
  "tha-ap139:8.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap139:9.1_0": "Itthaṁ sudaṁ āyasmā atthasandassako thero imā gāthāyo abhāsitthāti.",
  "tha-ap139:10.1_0": "Atthasandassakattherassāpadānaṁ sattamaṁ."
}