{
  "ja283:0.1_0": "Jātaka",
  "ja283:0.2_0": "Tikanipāta",
  "ja283:0.3_0": "Abbhantaravagga",
  "ja283:0.4_0": "3. Vaḍḍhakīsūkarajātaka",
  "ja283:1.1_0": "“Varaṁ varaṁ tvaṁ nihanaṁ pure cari,",
  "ja283:1.2_0": "Asmiṁ padese abhibhuyya sūkare;",
  "ja283:1.3_0": "So dāni eko byapagamma jhāyasi,",
  "ja283:1.4_0": "Balaṁ nu te byaggha na cajja vijjati”.",
  "ja283:2.1_0": "“Ime sudaṁ yanti disodisaṁ pure,",
  "ja283:2.2_0": "Bhayaṭṭitā leṇagavesino puthū;",
  "ja283:2.3_0": "Te dāni saṅgamma vasanti ekato,",
  "ja283:2.4_0": "Yatthaṭṭhitā duppasahajjame mayā”.",
  "ja283:3.1_0": "“Namatthu saṅghāna samāgatānaṁ,",
  "ja283:3.2_0": "Disvā sayaṁ sakhya vadāmi abbhutaṁ;",
  "ja283:3.3_0": "Byagghaṁ migā yattha jiniṁsu dāṭhino,",
  "ja283:3.4_0": "Sāmaggiyā dāṭhabalesu muccare”ti.",
  "ja283:4.1_0": "Vaḍḍhakīsūkarajātakaṁ tatiyaṁ."
}