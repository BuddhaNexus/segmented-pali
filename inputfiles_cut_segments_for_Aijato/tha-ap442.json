{
  "tha-ap442:0.1_0": "Therāpadāna",
  "tha-ap442:0.2_0": "Ekavihārivagga",
  "tha-ap442:0.3_0": "10 Ambaphaliyattheraapadāna",
  "tha-ap442:1.1_0": "“Padumuttarabuddhassa,",
  "tha-ap442:1.2_0": "lokajeṭṭhassa tādino;",
  "tha-ap442:1.3_0": "Piṇḍāya vicarantassa,",
  "tha-ap442:1.4_0": "dhārato uttamaṁ yasaṁ.",
  "tha-ap442:2.1_0": "Aggaphalaṁ gahetvāna,",
  "tha-ap442:2.2_0": "vippasannena cetasā;",
  "tha-ap442:2.3_0": "Dakkhiṇeyyassa vīrassa,",
  "tha-ap442:2.4_0": "adāsiṁ satthuno ahaṁ.",
  "tha-ap442:3.1_0": "Tena kammena dvipadinda,",
  "tha-ap442:3.2_0": "lokajeṭṭha narāsabha;",
  "tha-ap442:3.3_0": "Pattomhi acalaṁ ṭhānaṁ,",
  "tha-ap442:3.4_0": "hitvā jayaparājayaṁ.",
  "tha-ap442:4.1_0": "Satasahassito kappe,",
  "tha-ap442:4.2_0": "yaṁ dānamadadiṁ tadā;",
  "tha-ap442:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap442:4.4_0": "aggadānassidaṁ phalaṁ.",
  "tha-ap442:5.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap442:5.2_0": "…pe…",
  "tha-ap442:5.3_0": "viharāmi anāsavo.",
  "tha-ap442:6.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap442:6.2_0": "…pe…",
  "tha-ap442:6.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap442:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap442:7.2_0": "…pe…",
  "tha-ap442:7.3_0": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap442:8.1_0": "Itthaṁ sudaṁ āyasmā ambaphaliyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap442:9.1_0": "Ambaphaliyattherassāpadānaṁ dasamaṁ.",
  "tha-ap442:10.1_0": "Ekavihārivaggo catucattālīsamo.",
  "tha-ap442:11.1_0": "Tassuddānaṁ",
  "tha-ap442:12.1_0": "Thero ekavihārī ca,",
  "tha-ap442:12.2_0": "saṅkhiyo pāṭihīrako;",
  "tha-ap442:12.3_0": "Thaviko ucchukhaṇḍī ca,",
  "tha-ap442:12.4_0": "kaḷambaambāṭakado.",
  "tha-ap442:13.1_0": "Harītakambapiṇḍī ca,",
  "tha-ap442:13.2_0": "ambado dasamo yati;",
  "tha-ap442:13.3_0": "Chaḷasīti ca gāthāyo,",
  "tha-ap442:13.4_0": "gaṇitāyo vibhāvibhi."
}