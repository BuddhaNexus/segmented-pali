{
  "ja297:0.1_0": "Jātaka",
  "ja297:0.2_0": "Tikanipāta",
  "ja297:0.3_0": "Kumbhavagga",
  "ja297:0.4_0": "7. Kāmavilāpajātaka",
  "ja297:1.1_0": "“Ucce sakuṇa ḍemāna,",
  "ja297:1.2_0": "pattayāna vihaṅgama;",
  "ja297:1.3_0": "Vajjāsi kho tvaṁ vāmūruṁ,",
  "ja297:1.4_0": "ciraṁ kho sā karissati.",
  "ja297:2.1_0": "Idaṁ kho sā na jānāti,",
  "ja297:2.2_0": "asiṁ sattiñca oḍḍitaṁ;",
  "ja297:2.3_0": "Sā caṇḍī kāhati kodhaṁ,",
  "ja297:2.4_0": "taṁ me tapati no idaṁ.",
  "ja297:3.1_0": "Esa uppalasannāho,",
  "ja297:3.2_0": "nikkhañcussīsakohitaṁ;",
  "ja297:3.3_0": "Kāsikañca muduṁ vatthaṁ,",
  "ja297:3.4_0": "tappetu dhanikā piyā”ti.",
  "ja297:4.1_0": "Kāmavilāpajātakaṁ sattamaṁ."
}