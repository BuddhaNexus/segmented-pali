{
  "tha-ap368:0.1_0": "Therāpadāna",
  "tha-ap368:0.2_0": "Mandāravapupphiyavagga",
  "tha-ap368:0.3_0": "6. Kadambapupphiyattheraapadāna",
  "tha-ap368:1.1_0": "“Suvaṇṇavaṇṇaṁ sambuddhaṁ,",
  "tha-ap368:1.2_0": "Gacchantaṁ antarāpaṇe;",
  "tha-ap368:1.3_0": "Kañcanagghiyasaṅkāsaṁ,",
  "tha-ap368:1.4_0": "Bāttiṁsavaralakkhaṇaṁ.",
  "tha-ap368:2.1_0": "Nisajja pāsādavare,",
  "tha-ap368:2.2_0": "addasaṁ lokanāyakaṁ;",
  "tha-ap368:2.3_0": "Kadambapupphaṁ paggayha,",
  "tha-ap368:2.4_0": "vipassiṁ abhipūjayiṁ.",
  "tha-ap368:3.1_0": "Ekanavutito kappe,",
  "tha-ap368:3.2_0": "yaṁ buddhamabhipūjayiṁ;",
  "tha-ap368:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap368:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap368:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap368:4.2_0": "…pe…",
  "tha-ap368:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap368:5.1_0": "Itthaṁ sudaṁ āyasmā kadambapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap368:6.1_0": "Kadambapupphiyattherassāpadānaṁ chaṭṭhaṁ."
}