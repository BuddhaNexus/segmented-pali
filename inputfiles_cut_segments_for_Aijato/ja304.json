{
  "ja304:0.1_0": "Jātaka",
  "ja304:0.2_0": "Catukkanipāta",
  "ja304:0.3_0": "Kāliṅgavagga",
  "ja304:0.4_0": "4. Daddarajātaka",
  "ja304:1.1_0": "“Imāni maṁ daddara tāpayanti,",
  "ja304:1.2_0": "Vācāduruttāni manussaloke;",
  "ja304:1.3_0": "Maṇḍūkabhakkhā udakantasevī,",
  "ja304:1.4_0": "Āsīvisaṁ maṁ avisā sapanti”.",
  "ja304:2.1_0": "“Sakā raṭṭhā pabbājito,",
  "ja304:2.2_0": "aññaṁ janapadaṁ gato;",
  "ja304:2.3_0": "Mahantaṁ koṭṭhaṁ kayirātha,",
  "ja304:2.4_0": "duruttānaṁ nidhetave.",
  "ja304:3.1_0": "Yattha posaṁ na jānanti,",
  "ja304:3.2_0": "jātiyā vinayena vā;",
  "ja304:3.3_0": "Na tattha mānaṁ kayirātha,",
  "ja304:3.4_0": "vasamaññātake jane.",
  "ja304:4.1_0": "Videsavāsaṁ vasato,",
  "ja304:4.2_0": "jātavedasamenapi;",
  "ja304:4.3_0": "Khamitabbaṁ sapaññena,",
  "ja304:4.4_0": "api dāsassa tajjitan”ti.",
  "ja304:5.1_0": "Daddarajātakaṁ catutthaṁ."
}