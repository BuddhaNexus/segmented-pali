{
  "ja387:0.1_0": "Jātaka",
  "ja387:0.2_0": "Chakkanipāta",
  "ja387:0.3_0": "Kharaputtavagga",
  "ja387:0.4_0": "2. Sūcijātaka",
  "ja387:1.1_0": "“Akakkasaṁ apharusaṁ,",
  "ja387:1.2_0": "kharadhotaṁ supāsiyaṁ;",
  "ja387:1.3_0": "Sukhumaṁ tikhiṇaggañca,",
  "ja387:1.4_0": "ko sūciṁ ketumicchati.",
  "ja387:2.1_0": "Sumajjañca supāsañca,",
  "ja387:2.2_0": "anupubbaṁ suvaṭṭitaṁ;",
  "ja387:2.3_0": "Ghanaghātimaṁ paṭithaddhaṁ,",
  "ja387:2.4_0": "ko sūciṁ ketumicchati”.",
  "ja387:3.1_0": "“Ito dāni patāyanti,",
  "ja387:3.2_0": "sūciyo baḷisāni ca;",
  "ja387:3.3_0": "Koyaṁ kammāragāmasmiṁ,",
  "ja387:3.4_0": "sūciṁ vikketumicchati.",
  "ja387:4.1_0": "Ito satthāni gacchanti,",
  "ja387:4.2_0": "Kammantā vividhā puthū;",
  "ja387:4.3_0": "Koyaṁ kammāragāmasmiṁ,",
  "ja387:4.4_0": "Sūciṁ vikketumicchati”.",
  "ja387:5.1_0": "“Sūciṁ kammāragāmasmiṁ,",
  "ja387:5.2_0": "vikketabbā pajānatā;",
  "ja387:5.3_0": "Ācariyāva jānanti,",
  "ja387:5.4_0": "kammaṁ sukatadukkaṭaṁ.",
  "ja387:6.1_0": "Imañce te pitā bhadde,",
  "ja387:6.2_0": "Sūciṁ jaññā mayā kataṁ;",
  "ja387:6.3_0": "Tayā ca maṁ nimanteyya,",
  "ja387:6.4_0": "Yañcatthaññaṁ ghare dhanan”ti.",
  "ja387:7.1_0": "Sūcijātakaṁ dutiyaṁ."
}