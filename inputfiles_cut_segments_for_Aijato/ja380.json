{
  "ja380:0.1_0": "Jātaka",
  "ja380:0.2_0": "Chakkanipāta",
  "ja380:0.3_0": "Avāriyavagga",
  "ja380:0.4_0": "5. Āsaṅkajātaka",
  "ja380:1.1_0": "“Āsāvatī nāma latā,",
  "ja380:1.2_0": "jātā cittalatāvane;",
  "ja380:1.3_0": "Tassā vassasahassena,",
  "ja380:1.4_0": "ekaṁ nibbattate phalaṁ.",
  "ja380:2.1_0": "Taṁ devā payirupāsanti,",
  "ja380:2.2_0": "tāva dūraphalaṁ satiṁ;",
  "ja380:2.3_0": "Āsīseva tuvaṁ rāja,",
  "ja380:2.4_0": "āsā phalavatī sukhā.",
  "ja380:3.1_0": "Āsīsateva so pakkhī,",
  "ja380:3.2_0": "āsīsateva so dijo;",
  "ja380:3.3_0": "Tassa cāsā samijjhati,",
  "ja380:3.4_0": "tāva dūragatā satī;",
  "ja380:3.5_0": "Āsīseva tuvaṁ rāja,",
  "ja380:3.6_0": "āsā phalavatī sukhā”.",
  "ja380:4.1_0": "“Sampesi kho maṁ vācāya,",
  "ja380:4.2_0": "na ca sampesi kammunā;",
  "ja380:4.3_0": "Mālā sereyyakasseva,",
  "ja380:4.4_0": "vaṇṇavantā agandhikā.",
  "ja380:5.1_0": "Aphalaṁ madhuraṁ vācaṁ,",
  "ja380:5.2_0": "yo mittesu pakubbati;",
  "ja380:5.3_0": "Adadaṁ avissajaṁ bhogaṁ,",
  "ja380:5.4_0": "sandhi tenassa jīrati.",
  "ja380:6.1_0": "Yañhi kayirā tañhi vade,",
  "ja380:6.2_0": "yaṁ na kayirā na taṁ vade;",
  "ja380:6.3_0": "Akarontaṁ bhāsamānaṁ,",
  "ja380:6.4_0": "parijānanti paṇḍitā.",
  "ja380:7.1_0": "Balañca vata me khīṇaṁ,",
  "ja380:7.2_0": "pātheyyañca na vijjati;",
  "ja380:7.3_0": "Saṅke pāṇūparodhāya,",
  "ja380:7.4_0": "handa dāni vajāmahaṁ”.",
  "ja380:8.1_0": "“Etadeva hi me nāmaṁ,",
  "ja380:8.2_0": "Yaṁ nāmasmi rathesabha;",
  "ja380:8.3_0": "Āgamehi mahārāja,",
  "ja380:8.4_0": "Pitaraṁ āmantayāmahan”ti.",
  "ja380:9.1_0": "Āsaṅkajātakaṁ pañcamaṁ."
}