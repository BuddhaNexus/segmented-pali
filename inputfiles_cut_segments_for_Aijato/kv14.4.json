{
  "kv14.4:0.1_0": "Kathāvatthu",
  "kv14.4:0.2_0": "Mahāpaṇṇāsaka",
  "kv14.4:0.3_0": "Cuddasamavagga",
  "kv14.4:0.4_0": "Ariyarūpakathā",
  "kv14.4:1.1_0": "Ariyarūpaṁ mahābhūtānaṁ upādāyāti?",
  "kv14.4:1.2_0": "Āmantā.",
  "kv14.4:1.3_0": "Ariyarūpaṁ kusalanti?",
  "kv14.4:1.4_0": "Āmantā.",
  "kv14.4:1.5_0": "Mahābhūtā kusalāti?",
  "kv14.4:1.6_0": "Na hevaṁ vattabbe …pe…",
  "kv14.4:1.7_0": "mahābhūtā abyākatāti?",
  "kv14.4:1.8_0": "Āmantā.",
  "kv14.4:1.9_0": "Ariyarūpaṁ abyākatanti?",
  "kv14.4:1.10_0": "Na hevaṁ vattabbe …pe…",
  "kv14.4:1.11_0": "ariyarūpaṁ mahābhūtānaṁ upādāyāti?",
  "kv14.4:1.12_0": "Āmantā.",
  "kv14.4:1.13_0": "Ariyarūpaṁ anāsavaṁ asaṁyojaniyaṁ aganthaniyaṁ anoghaniyaṁ ayoganiyaṁ anīvaraṇiyaṁ aparāmaṭṭhaṁ anupādāniyaṁ asaṅkilesiyanti?",
  "kv14.4:1.14_0": "Āmantā.",
  "kv14.4:1.15_0": "Mahābhūtā anāsavā …pe… asaṅkilesiyāti?",
  "kv14.4:1.16_0": "Na hevaṁ vattabbe …pe…",
  "kv14.4:1.17_0": "mahābhūtā sāsavā saṁyojaniyā …pe… saṅkilesiyāti?",
  "kv14.4:1.18_0": "Āmantā.",
  "kv14.4:1.19_0": "Ariyarūpaṁ sāsavaṁ saṁyojaniyaṁ …pe… saṅkilesiyanti?",
  "kv14.4:1.20_0": "Na hevaṁ vattabbe …pe….",
  "kv14.4:2.1_0": "Na vattabbaṁ—",
  "kv14.4:2.2_0": "“ariyarūpaṁ mahābhūtānaṁ upādāyā”ti?",
  "kv14.4:2.3_0": "Āmantā.",
  "kv14.4:2.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv14.4:2.5_0": "“yaṁ kiñci, bhikkhave, rūpaṁ cattāri mahābhūtāni catunnañca mahābhūtānaṁ upādāyarūpan”ti.",
  "kv14.4:2.6_0": "Attheva suttantoti?",
  "kv14.4:2.7_0": "Āmantā.",
  "kv14.4:2.8_0": "Tena hi ariyarūpaṁ mahābhūtānaṁ upādāyāti.",
  "kv14.4:2.9_0": "Ariyarūpakathā niṭṭhitā."
}