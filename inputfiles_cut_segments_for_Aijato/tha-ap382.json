{
  "tha-ap382:0.1_0": "Therāpadāna",
  "tha-ap382:0.2_0": "Bodhivandanavagga",
  "tha-ap382:0.3_0": "10. Kāsumāriphaladāyakattheraapadāna",
  "tha-ap382:1.1_0": "“Kaṇikāraṁva jotantaṁ,",
  "tha-ap382:1.2_0": "nisinnaṁ pabbatantare;",
  "tha-ap382:1.3_0": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap382:1.4_0": "lokajeṭṭhaṁ narāsabhaṁ.",
  "tha-ap382:2.1_0": "Pasannacitto sumano,",
  "tha-ap382:2.2_0": "sire katvāna añjaliṁ;",
  "tha-ap382:2.3_0": "Kāsumāriphalaṁ gayha,",
  "tha-ap382:2.4_0": "buddhaseṭṭhassadāsahaṁ.",
  "tha-ap382:3.1_0": "Ekattiṁse ito kappe,",
  "tha-ap382:3.2_0": "yaṁ phalamadadiṁ ahaṁ;",
  "tha-ap382:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap382:3.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap382:4.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap382:4.2_0": "…pe…",
  "tha-ap382:4.3_0": "viharāmi anāsavo.",
  "tha-ap382:5.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap382:5.2_0": "…pe…",
  "tha-ap382:5.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap382:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap382:6.2_0": "…pe…",
  "tha-ap382:6.3_0": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap382:7.1_0": "Itthaṁ sudaṁ āyasmā kāsumāriphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap382:8.1_0": "Kāsumāriphaladāyakattherassāpadānaṁ dasamaṁ.",
  "tha-ap382:9.1_0": "Bodhivandanavaggo aṭṭhatiṁsatimo.",
  "tha-ap382:10.1_0": "Tassuddānaṁ",
  "tha-ap382:11.1_0": "Bodhi pāṭali uppalī,",
  "tha-ap382:11.2_0": "paṭṭi ca sattapaṇṇiyo;",
  "tha-ap382:11.3_0": "Gandhamuṭṭhi ca citako,",
  "tha-ap382:11.4_0": "tālaṁ sumanadāmako;",
  "tha-ap382:11.5_0": "Kāsumāriphalī ceva,",
  "tha-ap382:11.6_0": "gāthā ekūnasaṭṭhikā."
}