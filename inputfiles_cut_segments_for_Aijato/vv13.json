{
  "vv13:0.1_0": "Vimānavatthu",
  "vv13:0.2_0": "Itthivimāna",
  "vv13:0.3_0": "Pīṭhavagga",
  "vv13:0.4_0": "13. Paṭhamasuṇisāvimānavatthu",
  "vv13:1.1_0": "“Abhikkantena vaṇṇena,",
  "vv13:1.2_0": "yā tvaṁ tiṭṭhasi devate;",
  "vv13:1.3_0": "Obhāsentī disā sabbā,",
  "vv13:1.4_0": "osadhī viya tārakā.",
  "vv13:2.1_0": "Kena tetādiso vaṇṇo,",
  "vv13:2.2_0": "kena te idha mijjhati;",
  "vv13:2.3_0": "Uppajjanti ca te bhogā,",
  "vv13:2.4_0": "ye keci manaso piyā.",
  "vv13:3.1_0": "Pucchāmi taṁ devi mahānubhāve,",
  "vv13:3.2_0": "Manussabhūtā kimakāsi puññaṁ;",
  "vv13:3.3_0": "Kenāsi evaṁ jalitānubhāvā,",
  "vv13:3.4_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv13:4.1_0": "Sā devatā attamanā,",
  "vv13:4.2_0": "moggallānena pucchitā;",
  "vv13:4.3_0": "Pañhaṁ puṭṭhā viyākāsi,",
  "vv13:4.4_0": "yassa kammassidaṁ phalaṁ.",
  "vv13:5.1_0": "“Ahaṁ manussesu manussabhūtā,",
  "vv13:5.2_0": "Suṇisā ahosiṁ sasurassa gehe.",
  "vv13:6.1_0": "Addasaṁ virajaṁ bhikkhuṁ,",
  "vv13:6.2_0": "vippasannamanāvilaṁ;",
  "vv13:6.3_0": "Tassa adāsahaṁ pūvaṁ,",
  "vv13:6.4_0": "pasannā sehi pāṇibhi;",
  "vv13:6.5_0": "Bhāgaḍḍhabhāgaṁ datvāna,",
  "vv13:6.6_0": "modāmi nandane vane.",
  "vv13:7.1_0": "Tena metādiso vaṇṇo,",
  "vv13:7.2_0": "tena me idha mijjhati;",
  "vv13:7.3_0": "Uppajjanti ca me bhogā,",
  "vv13:7.4_0": "ye keci manaso piyā.",
  "vv13:8.1_0": "Akkhāmi te bhikkhu mahānubhāva,",
  "vv13:8.2_0": "Manussabhūtā yamakāsi puññaṁ;",
  "vv13:8.3_0": "Tenamhi evaṁ jalitānubhāvā,",
  "vv13:8.4_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv13:9.1_0": "Paṭhamasuṇisāvimānaṁ terasamaṁ."
}