{
  "ja379:0.1_0": "Jātaka",
  "ja379:0.2_0": "Chakkanipāta",
  "ja379:0.3_0": "Avāriyavagga",
  "ja379:0.4_0": "4. Nerujātaka",
  "ja379:1.1_0": "“Kākolā kākasaṅghā ca,",
  "ja379:1.2_0": "mayañca patataṁ varā;",
  "ja379:1.3_0": "Sabbeva sadisā homa,",
  "ja379:1.4_0": "imaṁ āgamma pabbataṁ.",
  "ja379:2.1_0": "Idha sīhā ca byagghā ca,",
  "ja379:2.2_0": "siṅgālā ca migādhamā;",
  "ja379:2.3_0": "Sabbeva sadisā honti,",
  "ja379:2.4_0": "ayaṁ ko nāma pabbato”.",
  "ja379:3.1_0": "“Imaṁ nerūti jānanti,",
  "ja379:3.2_0": "manussā pabbatuttamaṁ;",
  "ja379:3.3_0": "Idha vaṇṇena sampannā,",
  "ja379:3.4_0": "vasanti sabbapāṇino”.",
  "ja379:4.1_0": "“Amānanā yattha siyā,",
  "ja379:4.2_0": "santānaṁ vā vimānanā;",
  "ja379:4.3_0": "Hīnasammānanā vāpi,",
  "ja379:4.4_0": "na tattha vasatiṁ vase.",
  "ja379:5.1_0": "Yatthālaso ca dakkho ca,",
  "ja379:5.2_0": "sūro bhīru ca pūjiyā;",
  "ja379:5.3_0": "Na tattha santo vasanti,",
  "ja379:5.4_0": "avisesakare nare.",
  "ja379:6.1_0": "Nāyaṁ neru vibhajati,",
  "ja379:6.2_0": "hīnaukkaṭṭhamajjhime;",
  "ja379:6.3_0": "Avisesakaro neru,",
  "ja379:6.4_0": "handa neruṁ jahāmase”ti.",
  "ja379:7.1_0": "Nerujātakaṁ catutthaṁ."
}