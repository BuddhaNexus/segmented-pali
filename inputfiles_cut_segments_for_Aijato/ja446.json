{
  "ja446:0.1_0": "Jātaka",
  "ja446:0.2_0": "Dasakanipāta",
  "ja446:0.3_0": "Catudvāravagga",
  "ja446:0.4_0": "8. Takkalajātaka",
  "ja446:1.1_0": "“Na takkalā santi na āluvāni,",
  "ja446:1.2_0": "Na biḷāliyo na kaḷambāni tāta;",
  "ja446:1.3_0": "Eko araññamhi susānamajjhe,",
  "ja446:1.4_0": "Kimatthiko tāta khaṇāsi kāsuṁ”.",
  "ja446:2.1_0": "“Pitāmaho tāta sudubbalo te,",
  "ja446:2.2_0": "Anekabyādhīhi dukhena phuṭṭho;",
  "ja446:2.3_0": "Tamajjahaṁ nikhaṇissāmi sobbhe,",
  "ja446:2.4_0": "Na hissa taṁ jīvitaṁ rocayāmi”.",
  "ja446:3.1_0": "“Saṅkappametaṁ paṭiladdha pāpakaṁ,",
  "ja446:3.2_0": "Accāhitaṁ kamma karosi luddaṁ;",
  "ja446:3.3_0": "Mayāpi tāta paṭilacchase tuvaṁ,",
  "ja446:3.4_0": "Etādisaṁ kamma jarūpanīto;",
  "ja446:3.5_0": "Taṁ kullavattaṁ anuvattamāno,",
  "ja446:3.6_0": "Ahampi taṁ nikhaṇissāmi sobbhe”.",
  "ja446:4.1_0": "“Pharusāhi vācāhi pakubbamāno,",
  "ja446:4.2_0": "Āsajja maṁ tvaṁ vadase kumāra;",
  "ja446:4.3_0": "Putto mamaṁ orasako samāno,",
  "ja446:4.4_0": "Ahitānukampī mama tvaṁsi putta”.",
  "ja446:5.1_0": "“Na tāhaṁ tāta ahitānukampī,",
  "ja446:5.2_0": "Hitānukampī te ahampi tāta;",
  "ja446:5.3_0": "Pāpañca taṁ kamma pakubbamānaṁ,",
  "ja446:5.4_0": "Arahāmi no vārayituṁ tato”.",
  "ja446:6.1_0": "“Yo mātaraṁ vā pitaraṁ saviṭṭha,",
  "ja446:6.2_0": "Adūsake hiṁsati pāpadhammo;",
  "ja446:6.3_0": "Kāyassa bhedā abhisamparāyaṁ,",
  "ja446:6.4_0": "Asaṁsayaṁ so nirayaṁ upeti.",
  "ja446:7.1_0": "Yo mātaraṁ vā pitaraṁ saviṭṭha,",
  "ja446:7.2_0": "Annena pānena upaṭṭhahāti;",
  "ja446:7.3_0": "Kāyassa bhedā abhisamparāyaṁ,",
  "ja446:7.4_0": "Asaṁsayaṁ so sugatiṁ upeti.",
  "ja446:8.1_0": "Na me tvaṁ putta ahitānukampī,",
  "ja446:8.2_0": "Hitānukampī me tvaṁsi putta;",
  "ja446:8.3_0": "Ahañca taṁ mātarā vuccamāno,",
  "ja446:8.4_0": "Etādisaṁ kamma karomi luddaṁ”.",
  "ja446:9.1_0": "“Yā te sā bhariyā anariyarūpā,",
  "ja446:9.2_0": "Mātā mamesā sakiyā janetti;",
  "ja446:9.3_0": "Niddhāpaye tañca sakā agārā,",
  "ja446:9.4_0": "Aññampi te sā dukhamāvaheyya.",
  "ja446:10.1_0": "Yā te sā bhariyā anariyarūpā,",
  "ja446:10.2_0": "Mātā mamesā sakiyā janetti;",
  "ja446:10.3_0": "Dantā kareṇūva vasūpanītā,",
  "ja446:10.4_0": "Sā pāpadhammā punarāvajātū”ti.",
  "ja446:11.1_0": "Takkalajātakaṁ aṭṭhamaṁ."
}