{
  "ja529:0.1_0": "Jātaka",
  "ja529:0.2_0": "Saṭṭhinipāta",
  "ja529:0.3_0": "Soṇakavagga",
  "ja529:0.4_0": "1 Soṇakajātaka",
  "ja529:1.1_0": "“Tassa sutvā sataṁ dammi,",
  "ja529:1.2_0": "sahassaṁ diṭṭha soṇakaṁ;",
  "ja529:1.3_0": "Ko me soṇakamakkhāti,",
  "ja529:1.4_0": "sahāyaṁ paṁsukīḷitaṁ”.",
  "ja529:2.1_0": "“Athabravī māṇavako,",
  "ja529:2.2_0": "daharo pañcacūḷako;",
  "ja529:2.3_0": "Mayhaṁ sutvā sataṁ dehi,",
  "ja529:2.4_0": "sahassaṁ diṭṭha soṇakaṁ;",
  "ja529:2.5_0": "Ahaṁ te soṇakakkhissaṁ,",
  "ja529:2.6_0": "sahāyaṁ paṁsukīḷitaṁ”.",
  "ja529:3.1_0": "“Katamasmiṁ so janapade,",
  "ja529:3.2_0": "raṭṭhesu nigamesu ca;",
  "ja529:3.3_0": "Kattha soṇakamaddakkhi,",
  "ja529:3.4_0": "taṁ me akkhāhi pucchito”.",
  "ja529:4.1_0": "“Taveva deva vijite,",
  "ja529:4.2_0": "tavevuyyānabhūmiyā;",
  "ja529:4.3_0": "Ujuvaṁsā mahāsālā,",
  "ja529:4.4_0": "nīlobhāsā manoramā.",
  "ja529:5.1_0": "Tiṭṭhanti meghasamānā,",
  "ja529:5.2_0": "rammā aññoññanissitā;",
  "ja529:5.3_0": "Tesaṁ mūlamhi soṇako,",
  "ja529:5.4_0": "jhāyatī anupādano;",
  "ja529:5.5_0": "Upādānesu lokesu,",
  "ja529:5.6_0": "ḍayhamānesu nibbuto”.",
  "ja529:6.1_0": "Tato ca rājā pāyāsi,",
  "ja529:6.2_0": "senāya caturaṅgiyā;",
  "ja529:6.3_0": "Kārāpetvā samaṁ maggaṁ,",
  "ja529:6.4_0": "agamā yena soṇako.",
  "ja529:7.1_0": "Uyyānabhūmiṁ gantvāna,",
  "ja529:7.2_0": "vicaranto brahāvane;",
  "ja529:7.3_0": "Āsīnaṁ soṇakaṁ dakkhi,",
  "ja529:7.4_0": "ḍayhamānesu nibbutaṁ.",
  "ja529:8.1_0": "“Kapaṇo vatayaṁ bhikkhu,",
  "ja529:8.2_0": "muṇḍo saṅghāṭipāruto;",
  "ja529:8.3_0": "Amātiko apitiko,",
  "ja529:8.4_0": "rukkhamūlasmi jhāyati”.",
  "ja529:9.1_0": "Imaṁ vākyaṁ nisāmetvā,",
  "ja529:9.2_0": "Soṇako etadabravi;",
  "ja529:9.3_0": "“Na rāja kapaṇo hoti,",
  "ja529:9.4_0": "Dhammaṁ kāyena phassayaṁ.",
  "ja529:10.1_0": "Yo ca dhammaṁ niraṅkatvā,",
  "ja529:10.2_0": "Adhammamanuvattati;",
  "ja529:10.3_0": "Sa rāja kapaṇo hoti,",
  "ja529:10.4_0": "Pāpo pāpaparāyaṇo”.",
  "ja529:11.1_0": "“Arindamoti me nāmaṁ,",
  "ja529:11.2_0": "kāsirājāti maṁ vidū;",
  "ja529:11.3_0": "Kacci bhoto sukhasseyyā,",
  "ja529:11.4_0": "idha pattassa soṇaka”.",
  "ja529:12.1_0": "“Sadāpi bhadramadhanassa,",
  "ja529:12.2_0": "Anāgārassa bhikkhuno;",
  "ja529:12.3_0": "Na tesaṁ koṭṭhe openti,",
  "ja529:12.4_0": "Na kumbhiṁ na khaḷopiyaṁ;",
  "ja529:12.5_0": "Paraniṭṭhitamesānā,",
  "ja529:12.6_0": "Tena yāpenti subbatā.",
  "ja529:13.1_0": "Dutiyampi bhadramadhanassa,",
  "ja529:13.2_0": "Anāgārassa bhikkhuno;",
  "ja529:13.3_0": "Anavajjapiṇḍo bhottabbo,",
  "ja529:13.4_0": "Na ca kocūparodhati.",
  "ja529:14.1_0": "Tatiyampi bhadramadhanassa,",
  "ja529:14.2_0": "Anāgārassa bhikkhuno;",
  "ja529:14.3_0": "Nibbuto piṇḍo bhottabbo,",
  "ja529:14.4_0": "Na ca kocūparodhati.",
  "ja529:15.1_0": "Catutthampi bhadramadhanassa,",
  "ja529:15.2_0": "Anāgārassa bhikkhuno;",
  "ja529:15.3_0": "Muttassa raṭṭhe carato,",
  "ja529:15.4_0": "Saṅgo yassa na vijjati.",
  "ja529:16.1_0": "Pañcamampi bhadramadhanassa,",
  "ja529:16.2_0": "Anāgārassa bhikkhuno;",
  "ja529:16.3_0": "Nagaramhi ḍayhamānamhi,",
  "ja529:16.4_0": "Nāssa kiñci aḍayhatha.",
  "ja529:17.1_0": "Chaṭṭhampi bhadramadhanassa,",
  "ja529:17.2_0": "Anāgārassa bhikkhuno;",
  "ja529:17.3_0": "Raṭṭhe vilumpamānamhi,",
  "ja529:17.4_0": "Nāssa kiñci ahīratha.",
  "ja529:18.1_0": "Sattamampi bhadramadhanassa,",
  "ja529:18.2_0": "Anāgārassa bhikkhuno;",
  "ja529:18.3_0": "Corehi rakkhitaṁ maggaṁ,",
  "ja529:18.4_0": "Ye caññe paripanthikā;",
  "ja529:18.5_0": "Pattacīvaramādāya,",
  "ja529:18.6_0": "Sotthiṁ gacchati subbato.",
  "ja529:19.1_0": "Aṭṭhamampi bhadramadhanassa,",
  "ja529:19.2_0": "Anāgārassa bhikkhuno;",
  "ja529:19.3_0": "Yaṁ yaṁ disaṁ pakkamati,",
  "ja529:19.4_0": "Anapekkhova gacchati”.",
  "ja529:20.1_0": "“Bahūni samaṇabhadrāni,",
  "ja529:20.2_0": "ye tvaṁ bhikkhu pasaṁsasi;",
  "ja529:20.3_0": "Ahañca giddho kāmesu,",
  "ja529:20.4_0": "kathaṁ kāhāmi soṇaka.",
  "ja529:21.1_0": "Piyā me mānusā kāmā,",
  "ja529:21.2_0": "atho dibyāpi me piyā;",
  "ja529:21.3_0": "Atha kena nu vaṇṇena,",
  "ja529:21.4_0": "ubho loke labhāmase”.",
  "ja529:22.1_0": "“Kāme giddhā kāmaratā,",
  "ja529:22.2_0": "kāmesu adhimucchitā;",
  "ja529:22.3_0": "Narā pāpāni katvāna,",
  "ja529:22.4_0": "upapajjanti duggatiṁ.",
  "ja529:23.1_0": "Ye ca kāme pahantvāna,",
  "ja529:23.2_0": "nikkhantā akutobhayā;",
  "ja529:23.3_0": "Ekodibhāvādhigatā,",
  "ja529:23.4_0": "na te gacchanti duggatiṁ.",
  "ja529:24.1_0": "Upamaṁ te karissāmi,",
  "ja529:24.2_0": "taṁ suṇohi arindama;",
  "ja529:24.3_0": "Upamāya midhekacce,",
  "ja529:24.4_0": "atthaṁ jānanti paṇḍitā.",
  "ja529:25.1_0": "Gaṅgāya kuṇapaṁ disvā,",
  "ja529:25.2_0": "vuyhamānaṁ mahaṇṇave;",
  "ja529:25.3_0": "Vāyaso samacintesi,",
  "ja529:25.4_0": "appapañño acetaso.",
  "ja529:26.1_0": "‘Yānañca vatidaṁ laddhaṁ,",
  "ja529:26.2_0": "bhakkho cāyaṁ anappako’;",
  "ja529:26.3_0": "Tattha rattiṁ tattha divā,",
  "ja529:26.4_0": "tattheva nirato mano.",
  "ja529:27.1_0": "Khādaṁ nāgassa maṁsāni,",
  "ja529:27.2_0": "pivaṁ bhāgīrathodakaṁ;",
  "ja529:27.3_0": "Sampassaṁ vanacetyāni,",
  "ja529:27.4_0": "na palettha vihaṅgamo.",
  "ja529:28.1_0": "Tañca otaraṇī gaṅgā,",
  "ja529:28.2_0": "pamattaṁ kuṇape rataṁ;",
  "ja529:28.3_0": "Samuddaṁ ajjhagāhāsi,",
  "ja529:28.4_0": "agatī yattha pakkhinaṁ.",
  "ja529:29.1_0": "So ca bhakkhaparikkhīṇo,",
  "ja529:29.2_0": "udapatvā vihaṅgamo;",
  "ja529:29.3_0": "Na pacchato na purato,",
  "ja529:29.4_0": "nuttaraṁ nopi dakkhiṇaṁ.",
  "ja529:30.1_0": "Dīpaṁ so najjhagāgañchi,",
  "ja529:30.2_0": "agatī yattha pakkhinaṁ;",
  "ja529:30.3_0": "So ca tattheva pāpattha,",
  "ja529:30.4_0": "yathā dubbalako tathā.",
  "ja529:31.1_0": "Tañca sāmuddikā macchā,",
  "ja529:31.2_0": "kumbhīlā makarā susū;",
  "ja529:31.3_0": "Pasayhakārā khādiṁsu,",
  "ja529:31.4_0": "phandamānaṁ vipakkhakaṁ.",
  "ja529:32.1_0": "Evameva tuvaṁ rāja,",
  "ja529:32.2_0": "ye caññe kāmabhogino;",
  "ja529:32.3_0": "Giddhā ce na vamissanti,",
  "ja529:32.4_0": "kākapaññāva te vidū.",
  "ja529:33.1_0": "Esā te upamā rāja,",
  "ja529:33.2_0": "atthasandassanī katā;",
  "ja529:33.3_0": "Tvañca paññāyase tena,",
  "ja529:33.4_0": "yadi kāhasi vā na vā.",
  "ja529:34.1_0": "Ekavācampi dvivācaṁ,",
  "ja529:34.2_0": "bhaṇeyya anukampako;",
  "ja529:34.3_0": "Tatuttariṁ na bhāseyya,",
  "ja529:34.4_0": "dāsovayyassa santike”.",
  "ja529:35.1_0": "Idaṁ vatvāna pakkāmi,",
  "ja529:35.2_0": "soṇako amitabuddhimā;",
  "ja529:35.3_0": "Vehāse antalikkhasmiṁ,",
  "ja529:35.4_0": "anusāsitvāna khattiyaṁ.",
  "ja529:36.1_0": "“Ko nume rājakattāro,",
  "ja529:36.2_0": "suddā veyyattamāgatā;",
  "ja529:36.3_0": "Rajjaṁ niyyādayissāmi,",
  "ja529:36.4_0": "nāhaṁ rajjena matthiko.",
  "ja529:37.1_0": "Ajjeva pabbajissāmi,",
  "ja529:37.2_0": "Ko jaññā maraṇaṁ suve;",
  "ja529:37.3_0": "Māhaṁ kākova dummedho,",
  "ja529:37.4_0": "Kāmānaṁ vasamanvagaṁ”.",
  "ja529:38.1_0": "“Atthi te daharo putto,",
  "ja529:38.2_0": "dīghāvu raṭṭhavaḍḍhano;",
  "ja529:38.3_0": "Taṁ rajje abhisiñcassu,",
  "ja529:38.4_0": "so no rājā bhavissati”.",
  "ja529:39.1_0": "“Khippaṁ kumāramānetha,",
  "ja529:39.2_0": "dīghāvuṁ raṭṭhavaḍḍhanaṁ;",
  "ja529:39.3_0": "Taṁ rajje abhisiñcissaṁ,",
  "ja529:39.4_0": "so vo rājā bhavissati”.",
  "ja529:40.1_0": "Tato kumāramānesuṁ,",
  "ja529:40.2_0": "dīghāvuṁ raṭṭhavaḍḍhanaṁ;",
  "ja529:40.3_0": "Taṁ disvā ālapī rājā,",
  "ja529:40.4_0": "ekaputtaṁ manoramaṁ.",
  "ja529:41.1_0": "“Saṭṭhi gāmasahassāni,",
  "ja529:41.2_0": "paripuṇṇāni sabbaso;",
  "ja529:41.3_0": "Te putta paṭipajjassu,",
  "ja529:41.4_0": "rajjaṁ niyyādayāmi te.",
  "ja529:42.1_0": "Ajjeva pabbajissāmi,",
  "ja529:42.2_0": "ko jaññā maraṇaṁ suve;",
  "ja529:42.3_0": "Māhaṁ kākova dummedho,",
  "ja529:42.4_0": "kāmānaṁ vasamanvagaṁ.",
  "ja529:43.1_0": "Saṭṭhi nāgasahassāni,",
  "ja529:43.2_0": "sabbālaṅkārabhūsitā;",
  "ja529:43.3_0": "Suvaṇṇakacchā mātaṅgā,",
  "ja529:43.4_0": "hemakappanavāsasā.",
  "ja529:44.1_0": "Ārūḷhā gāmaṇīyehi,",
  "ja529:44.2_0": "tomaraṅkusapāṇibhi;",
  "ja529:44.3_0": "Te putta paṭipajjassu,",
  "ja529:44.4_0": "rajjaṁ niyyādayāmi te.",
  "ja529:45.1_0": "Ajjeva pabbajissāmi,",
  "ja529:45.2_0": "ko jaññā maraṇaṁ suve;",
  "ja529:45.3_0": "Māhaṁ kākova dummedho,",
  "ja529:45.4_0": "kāmānaṁ vasamanvagaṁ.",
  "ja529:46.1_0": "Saṭṭhi assasahassāni,",
  "ja529:46.2_0": "sabbālaṅkārabhūsitā;",
  "ja529:46.3_0": "Ājānīyāva jātiyā,",
  "ja529:46.4_0": "sindhavā sīghavāhino.",
  "ja529:47.1_0": "Ārūḷhā gāmaṇīyehi,",
  "ja529:47.2_0": "illiyācāpadhāribhi;",
  "ja529:47.3_0": "Te putta paṭipajjassu,",
  "ja529:47.4_0": "rajjaṁ niyyādayāmi te.",
  "ja529:48.1_0": "Ajjeva pabbajissāmi,",
  "ja529:48.2_0": "ko jaññā maraṇaṁ suve;",
  "ja529:48.3_0": "Māhaṁ kākova dummedho,",
  "ja529:48.4_0": "kāmānaṁ vasamanvagaṁ.",
  "ja529:49.1_0": "Saṭṭhi rathasahassāni,",
  "ja529:49.2_0": "sannaddhā ussitaddhajā;",
  "ja529:49.3_0": "Dīpā athopi veyyagghā,",
  "ja529:49.4_0": "sabbālaṅkārabhūsitā.",
  "ja529:50.1_0": "Ārūḷhā gāmaṇīyehi,",
  "ja529:50.2_0": "cāpahatthehi vammibhi;",
  "ja529:50.3_0": "Te putta paṭipajjassu,",
  "ja529:50.4_0": "rajjaṁ niyyādayāmi te.",
  "ja529:51.1_0": "Ajjeva pabbajissāmi,",
  "ja529:51.2_0": "ko jaññā maraṇaṁ suve;",
  "ja529:51.3_0": "Māhaṁ kākova dummedho,",
  "ja529:51.4_0": "kāmānaṁ vasamanvagaṁ.",
  "ja529:52.1_0": "Saṭṭhi dhenusahassāni,",
  "ja529:52.2_0": "rohaññā puṅgavūsabhā;",
  "ja529:52.3_0": "Tā putta paṭipajjassu,",
  "ja529:52.4_0": "rajjaṁ niyyādayāmi te.",
  "ja529:53.1_0": "Ajjeva pabbajissāmi,",
  "ja529:53.2_0": "ko jaññā maraṇaṁ suve;",
  "ja529:53.3_0": "Māhaṁ kākova dummedho,",
  "ja529:53.4_0": "kāmānaṁ vasamanvagaṁ.",
  "ja529:54.1_0": "Soḷasitthisahassāni,",
  "ja529:54.2_0": "sabbālaṅkārabhūsitā;",
  "ja529:54.3_0": "Vicitravatthābharaṇā,",
  "ja529:54.4_0": "āmuttamaṇikuṇḍalā;",
  "ja529:54.5_0": "Tā putta paṭipajjassu,",
  "ja529:54.6_0": "rajjaṁ niyyādayāmi te.",
  "ja529:55.1_0": "Ajjeva pabbajissāmi,",
  "ja529:55.2_0": "ko jaññā maraṇaṁ suve;",
  "ja529:55.3_0": "Māhaṁ kākova dummedho,",
  "ja529:55.4_0": "kāmānaṁ vasamanvagaṁ”.",
  "ja529:56.1_0": "“Daharasseva me tāta,",
  "ja529:56.2_0": "mātā matāti me sutaṁ;",
  "ja529:56.3_0": "Tayā vinā ahaṁ tāta,",
  "ja529:56.4_0": "jīvitumpi na ussahe.",
  "ja529:57.1_0": "Yathā āraññakaṁ nāgaṁ,",
  "ja529:57.2_0": "poto anveti pacchato;",
  "ja529:57.3_0": "Jessantaṁ giriduggesu,",
  "ja529:57.4_0": "samesu visamesu ca.",
  "ja529:58.1_0": "Evaṁ taṁ anugacchāmi,",
  "ja529:58.2_0": "pattamādāya pacchato;",
  "ja529:58.3_0": "Subharo te bhavissāmi,",
  "ja529:58.4_0": "na te hessāmi dubbharo”.",
  "ja529:59.1_0": "“Yathā sāmuddikaṁ nāvaṁ,",
  "ja529:59.2_0": "vāṇijānaṁ dhanesinaṁ;",
  "ja529:59.3_0": "Vohāro tattha gaṇheyya,",
  "ja529:59.4_0": "vāṇijā byasanī siyā.",
  "ja529:60.1_0": "Evamevāyaṁ puttakali,",
  "ja529:60.2_0": "antarāyakaro mama;",
  "ja529:60.3_0": "Imaṁ kumāraṁ pāpetha,",
  "ja529:60.4_0": "pāsādaṁ rativaḍḍhanaṁ.",
  "ja529:61.1_0": "Tattha kambusahatthāyo,",
  "ja529:61.2_0": "yathā sakkaṁva accharā;",
  "ja529:61.3_0": "Tā naṁ tattha ramessanti,",
  "ja529:61.4_0": "tāhi ceso ramissati.",
  "ja529:62.1_0": "Tato kumāraṁ pāpesuṁ,",
  "ja529:62.2_0": "pāsādaṁ rativaḍḍhanaṁ;",
  "ja529:62.3_0": "Taṁ disvā avacuṁ kaññā,",
  "ja529:62.4_0": "dīghāvuṁ raṭṭhavaḍḍhanaṁ.",
  "ja529:63.1_0": "Devatā nusi gandhabbo,",
  "ja529:63.2_0": "adu sakko purindado;",
  "ja529:63.3_0": "Ko vā tvaṁ kassa vā putto,",
  "ja529:63.4_0": "kathaṁ jānemu taṁ mayaṁ”.",
  "ja529:64.1_0": "“Namhi devo na gandhabbo,",
  "ja529:64.2_0": "Nāpi sakko purindado;",
  "ja529:64.3_0": "Kāsirañño ahaṁ putto,",
  "ja529:64.4_0": "Dīghāvu raṭṭhavaḍḍhano;",
  "ja529:64.5_0": "Mamaṁ bharatha bhaddaṁ vo,",
  "ja529:64.6_0": "Ahaṁ bhattā bhavāmi vo”.",
  "ja529:65.1_0": "Taṁ tattha avacuṁ kaññā,",
  "ja529:65.2_0": "dīghāvuṁ raṭṭhavaḍḍhanaṁ;",
  "ja529:65.3_0": "“Kuhiṁ rājā anuppatto,",
  "ja529:65.4_0": "ito rājā kuhiṁ gato”.",
  "ja529:66.1_0": "“Paṅkaṁ rājā atikkanto,",
  "ja529:66.2_0": "thale rājā patiṭṭhito;",
  "ja529:66.3_0": "Akaṇṭakaṁ agahanaṁ,",
  "ja529:66.4_0": "paṭipanno mahāpathaṁ.",
  "ja529:67.1_0": "Ahañca paṭipannosmi,",
  "ja529:67.2_0": "maggaṁ duggatigāminaṁ;",
  "ja529:67.3_0": "Sakaṇṭakaṁ sagahanaṁ,",
  "ja529:67.4_0": "yena gacchanti duggatiṁ”.",
  "ja529:68.1_0": "“Tassa te svāgataṁ rāja,",
  "ja529:68.2_0": "sīhasseva giribbajaṁ;",
  "ja529:68.3_0": "Anusāsa mahārāja,",
  "ja529:68.4_0": "tvaṁ no sabbāsamissaro”ti.",
  "ja529:69.1_0": "Soṇakajātakaṁ paṭhamaṁ."
}