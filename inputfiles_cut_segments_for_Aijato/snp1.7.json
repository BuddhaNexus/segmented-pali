{
  "snp1.7:0.1_0": "Sutta Nipāta 1.7",
  "snp1.7:0.2_0": "Vasalasutta",
  "snp1.7:1.1_0": "Evaṁ me sutaṁ—",
  "snp1.7:1.2_0": "ekaṁ samayaṁ bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "snp1.7:1.3_0": "Atha kho bhagavā pubbaṇhasamayaṁ nivāsetvā pattacīvaramādāya sāvatthiṁ piṇḍāya pāvisi.",
  "snp1.7:1.4_0": "Tena kho pana samayena aggikabhāradvājassa brāhmaṇassa nivesane aggi pajjalito hoti āhuti paggahitā.",
  "snp1.7:1.5_0": "Atha kho bhagavā sāvatthiyaṁ sapadānaṁ piṇḍāya caramāno yena aggikabhāradvājassa brāhmaṇassa nivesanaṁ tenupasaṅkami.",
  "snp1.7:2.1_0": "Addasā kho aggikabhāradvājo brāhmaṇo bhagavantaṁ dūratova āgacchantaṁ.",
  "snp1.7:2.2_0": "Disvāna bhagavantaṁ etadavoca:",
  "snp1.7:2.3_0": "“tatreva, muṇḍaka;",
  "snp1.7:2.4_0": "tatreva, samaṇaka;",
  "snp1.7:2.5_0": "tatreva, vasalaka, tiṭṭhāhī”ti.",
  "snp1.7:3.1_0": "Evaṁ vutte, bhagavā aggikabhāradvājaṁ brāhmaṇaṁ etadavoca:",
  "snp1.7:3.2_0": "“jānāsi pana tvaṁ, brāhmaṇa, vasalaṁ vā vasalakaraṇe vā dhamme”ti?",
  "snp1.7:3.3_0": "“Na khvāhaṁ, bho gotama, jānāmi vasalaṁ vā vasalakaraṇe vā dhamme;",
  "snp1.7:3.4_0": "sādhu me bhavaṁ gotamo tathā dhammaṁ desetu, yathāhaṁ jāneyyaṁ vasalaṁ vā vasalakaraṇe vā dhamme”ti.",
  "snp1.7:3.5_0": "“Tena hi, brāhmaṇa, suṇāhi, sādhukaṁ manasi karohi; bhāsissāmī”ti.",
  "snp1.7:3.6_0": "“Evaṁ, bho”ti kho aggikabhāradvājo brāhmaṇo bhagavato paccassosi.",
  "snp1.7:3.7_0": "Bhagavā etadavoca:",
  "snp1.7:4.1_0": "“Kodhano upanāhī ca,",
  "snp1.7:4.2_0": "pāpamakkhī ca yo naro;",
  "snp1.7:4.3_0": "Vipannadiṭṭhi māyāvī,",
  "snp1.7:4.4_0": "taṁ jaññā vasalo iti.",
  "snp1.7:5.1_0": "Ekajaṁ vā dvijaṁ vāpi,",
  "snp1.7:5.2_0": "yodha pāṇaṁ vihiṁsati;",
  "snp1.7:5.3_0": "Yassa pāṇe dayā natthi,",
  "snp1.7:5.4_0": "taṁ jaññā vasalo iti.",
  "snp1.7:6.1_0": "Yo hanti parirundhati,",
  "snp1.7:6.2_0": "gāmāni nigamāni ca;",
  "snp1.7:6.3_0": "Niggāhako samaññāto,",
  "snp1.7:6.4_0": "taṁ jaññā vasalo iti.",
  "snp1.7:7.1_0": "Gāme vā yadi vā raññe,",
  "snp1.7:7.2_0": "yaṁ paresaṁ mamāyitaṁ;",
  "snp1.7:7.3_0": "Theyyā adinnamādeti,",
  "snp1.7:7.4_0": "taṁ jaññā vasalo iti.",
  "snp1.7:8.1_0": "Yo have iṇamādāya,",
  "snp1.7:8.2_0": "cujjamāno palāyati;",
  "snp1.7:8.3_0": "Na hi te iṇamatthīti,",
  "snp1.7:8.4_0": "taṁ jaññā vasalo iti.",
  "snp1.7:9.1_0": "Yo ve kiñcikkhakamyatā,",
  "snp1.7:9.2_0": "panthasmiṁ vajantaṁ janaṁ;",
  "snp1.7:9.3_0": "Hantvā kiñcikkhamādeti,",
  "snp1.7:9.4_0": "taṁ jaññā vasalo iti.",
  "snp1.7:10.1_0": "Attahetu parahetu,",
  "snp1.7:10.2_0": "dhanahetu ca yo naro;",
  "snp1.7:10.3_0": "Sakkhipuṭṭho musā brūti,",
  "snp1.7:10.4_0": "taṁ jaññā vasalo iti.",
  "snp1.7:11.1_0": "Yo ñātīnaṁ sakhīnaṁ vā,",
  "snp1.7:11.2_0": "dāresu paṭidissati;",
  "snp1.7:11.3_0": "Sāhasā sampiyena vā,",
  "snp1.7:11.4_0": "taṁ jaññā vasalo iti.",
  "snp1.7:12.1_0": "Yo mātaraṁ pitaraṁ vā,",
  "snp1.7:12.2_0": "jiṇṇakaṁ gatayobbanaṁ;",
  "snp1.7:12.3_0": "Pahu santo na bharati,",
  "snp1.7:12.4_0": "taṁ jaññā vasalo iti.",
  "snp1.7:13.1_0": "Yo mātaraṁ pitaraṁ vā,",
  "snp1.7:13.2_0": "Bhātaraṁ bhaginiṁ sasuṁ;",
  "snp1.7:13.3_0": "Hanti roseti vācāya,",
  "snp1.7:13.4_0": "Taṁ jaññā vasalo iti.",
  "snp1.7:14.1_0": "Yo atthaṁ pucchito santo,",
  "snp1.7:14.2_0": "Anatthamanusāsati;",
  "snp1.7:14.3_0": "Paṭicchannena manteti,",
  "snp1.7:14.4_0": "Taṁ jaññā vasalo iti.",
  "snp1.7:15.1_0": "Yo katvā pāpakaṁ kammaṁ,",
  "snp1.7:15.2_0": "Mā maṁ jaññāti icchati;",
  "snp1.7:15.3_0": "Yo paṭicchannakammanto,",
  "snp1.7:15.4_0": "Taṁ jaññā vasalo iti.",
  "snp1.7:16.1_0": "Yo ve parakulaṁ gantvā,",
  "snp1.7:16.2_0": "Bhutvāna sucibhojanaṁ;",
  "snp1.7:16.3_0": "Āgataṁ nappaṭipūjeti,",
  "snp1.7:16.4_0": "Taṁ jaññā vasalo iti.",
  "snp1.7:17.1_0": "Yo brāhmaṇaṁ samaṇaṁ vā,",
  "snp1.7:17.2_0": "Aññaṁ vāpi vanibbakaṁ;",
  "snp1.7:17.3_0": "Musāvādena vañceti,",
  "snp1.7:17.4_0": "Taṁ jaññā vasalo iti.",
  "snp1.7:18.1_0": "Yo brāhmaṇaṁ samaṇaṁ vā,",
  "snp1.7:18.2_0": "Bhattakāle upaṭṭhite;",
  "snp1.7:18.3_0": "Roseti vācā na ca deti,",
  "snp1.7:18.4_0": "Taṁ jaññā vasalo iti.",
  "snp1.7:19.1_0": "Asataṁ yodha pabrūti,",
  "snp1.7:19.2_0": "Mohena paliguṇṭhito;",
  "snp1.7:19.3_0": "Kiñcikkhaṁ nijigīsāno,",
  "snp1.7:19.4_0": "Taṁ jaññā vasalo iti.",
  "snp1.7:20.1_0": "Yo cattānaṁ samukkaṁse,",
  "snp1.7:20.2_0": "Pare ca mavajānāti;",
  "snp1.7:20.3_0": "Nihīno sena mānena,",
  "snp1.7:20.4_0": "Taṁ jaññā vasalo iti.",
  "snp1.7:21.1_0": "Rosako kadariyo ca,",
  "snp1.7:21.2_0": "Pāpiccho maccharī saṭho;",
  "snp1.7:21.3_0": "Ahiriko anottappī,",
  "snp1.7:21.4_0": "Taṁ jaññā vasalo iti.",
  "snp1.7:22.1_0": "Yo buddhaṁ paribhāsati,",
  "snp1.7:22.2_0": "Atha vā tassa sāvakaṁ;",
  "snp1.7:22.3_0": "Paribbājaṁ gahaṭṭhaṁ vā,",
  "snp1.7:22.4_0": "Taṁ jaññā vasalo iti.",
  "snp1.7:23.1_0": "Yo ve anarahaṁ santo,",
  "snp1.7:23.2_0": "Arahaṁ paṭijānāti;",
  "snp1.7:23.3_0": "Coro sabrahmake loke,",
  "snp1.7:23.4_0": "Eso kho vasalādhamo;",
  "snp1.7:23.5_0": "Ete kho vasalā vuttā,",
  "snp1.7:23.6_0": "Mayā yete pakāsitā.",
  "snp1.7:24.1_0": "Na jaccā vasalo hoti,",
  "snp1.7:24.2_0": "Na jaccā hoti brāhmaṇo;",
  "snp1.7:24.3_0": "Kammunā vasalo hoti,",
  "snp1.7:24.4_0": "Kammunā hoti brāhmaṇo.",
  "snp1.7:25.1_0": "Tadamināpi jānātha,",
  "snp1.7:25.2_0": "Yathāmedaṁ nidassanaṁ;",
  "snp1.7:25.3_0": "Caṇḍālaputto sopāko,",
  "snp1.7:25.4_0": "Mātaṅgo iti vissuto.",
  "snp1.7:26.1_0": "So yasaṁ paramaṁ patto,",
  "snp1.7:26.2_0": "Mātaṅgo yaṁ sudullabhaṁ;",
  "snp1.7:26.3_0": "Āgacchuṁ tassupaṭṭhānaṁ,",
  "snp1.7:26.4_0": "Khattiyā brāhmaṇā bahū.",
  "snp1.7:27.1_0": "Devayānaṁ abhiruyha,",
  "snp1.7:27.2_0": "Virajaṁ so mahāpathaṁ;",
  "snp1.7:27.3_0": "Kāmarāgaṁ virājetvā,",
  "snp1.7:27.4_0": "Brahmalokūpago ahu;",
  "snp1.7:27.5_0": "Na naṁ jāti nivāresi,",
  "snp1.7:27.6_0": "Brahmalokūpapattiyā.",
  "snp1.7:28.1_0": "Ajjhāyakakule jātā,",
  "snp1.7:28.2_0": "Brāhmaṇā mantabandhavā;",
  "snp1.7:28.3_0": "Te ca pāpesu kammesu,",
  "snp1.7:28.4_0": "Abhiṇhamupadissare.",
  "snp1.7:29.1_0": "Diṭṭheva dhamme gārayhā,",
  "snp1.7:29.2_0": "Samparāye ca duggati;",
  "snp1.7:29.3_0": "Na ne jāti nivāreti,",
  "snp1.7:29.4_0": "Duggatyā garahāya vā.",
  "snp1.7:30.1_0": "Na jaccā vasalo hoti,",
  "snp1.7:30.2_0": "Na jaccā hoti brāhmaṇo;",
  "snp1.7:30.3_0": "Kammunā vasalo hoti,",
  "snp1.7:30.4_0": "Kammunā hoti brāhmaṇo”ti.",
  "snp1.7:31.1_0": "Evaṁ vutte, aggikabhāradvājo brāhmaṇo bhagavantaṁ etadavoca:",
  "snp1.7:31.2_0": "“abhikkantaṁ, bho gotama …pe…",
  "snp1.7:31.3_0": "upāsakaṁ maṁ bhavaṁ gotamo dhāretu ajjatagge pāṇupetaṁ saraṇaṁ gatan”ti.",
  "snp1.7:31.4_0": "Vasalasuttaṁ sattamaṁ."
}