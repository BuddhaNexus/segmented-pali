{
  "kv21.1:0.1_0": "Kathāvatthu",
  "kv21.1:0.2_0": "Khuddakaaḍḍhapaṇṇāsaka",
  "kv21.1:0.3_0": "Ekavīsatimavagga",
  "kv21.1:0.4_0": "Sāsanakathā",
  "kv21.1:1.1_0": "Sāsanaṁ navaṁ katanti?",
  "kv21.1:1.2_0": "Āmantā.",
  "kv21.1:1.3_0": "Satipaṭṭhānā navaṁ katāti?",
  "kv21.1:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv21.1:1.5_0": "sāsanaṁ navaṁ katanti?",
  "kv21.1:1.6_0": "Āmantā.",
  "kv21.1:1.7_0": "Sammappadhānā …pe…",
  "kv21.1:1.8_0": "iddhipādā …pe…",
  "kv21.1:1.9_0": "indriyā …pe…",
  "kv21.1:1.10_0": "balā …pe…",
  "kv21.1:1.11_0": "bojjhaṅgā navaṁ katāti?",
  "kv21.1:1.12_0": "Na hevaṁ vattabbe …pe…",
  "kv21.1:1.13_0": "pubbe akusalaṁ pacchā kusalaṁ katanti?",
  "kv21.1:1.14_0": "Na hevaṁ vattabbe …pe…",
  "kv21.1:1.15_0": "pubbe sāsavaṁ …pe…",
  "kv21.1:1.15_1": "saṁyojaniyaṁ ganthaniyaṁ oghaniyaṁ yoganiyaṁ nīvaraṇiyaṁ parāmaṭṭhaṁ upādāniyaṁ …pe…",
  "kv21.1:1.15_2": "saṅkilesiyaṁ pacchā asaṅkilesiyaṁ katanti?",
  "kv21.1:1.16_0": "Na hevaṁ vattabbe …pe….",
  "kv21.1:2.1_0": "Atthi koci tathāgatassa sāsanaṁ navaṁ karotīti?",
  "kv21.1:2.2_0": "Āmantā.",
  "kv21.1:2.3_0": "Atthi koci satipaṭṭhāne navaṁ karotīti?",
  "kv21.1:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv21.1:2.5_0": "atthi koci sammappadhāne …pe…",
  "kv21.1:2.6_0": "iddhipāde …pe…",
  "kv21.1:2.7_0": "indriye …pe…",
  "kv21.1:2.8_0": "bale …pe…",
  "kv21.1:2.9_0": "bojjhaṅge navaṁ karotīti?",
  "kv21.1:2.10_0": "Na hevaṁ vattabbe …pe…",
  "kv21.1:2.11_0": "atthi koci pubbe akusalaṁ pacchā kusalaṁ karotīti?",
  "kv21.1:2.12_0": "Na hevaṁ vattabbe …pe…",
  "kv21.1:2.13_0": "atthi koci pubbe sāsavaṁ …pe… saṅkilesiyaṁ pacchā asaṅkilesiyaṁ karotīti?",
  "kv21.1:2.14_0": "Na hevaṁ vattabbe …pe….",
  "kv21.1:3.1_0": "Labbhā tathāgatassa sāsanaṁ puna navaṁ kātunti?",
  "kv21.1:3.2_0": "Āmantā.",
  "kv21.1:3.3_0": "Labbhā satipaṭṭhānā puna navaṁ kātunti?",
  "kv21.1:3.4_0": "Na hevaṁ vattabbe …pe…",
  "kv21.1:3.5_0": "labbhā sammappadhānā …pe…",
  "kv21.1:3.6_0": "iddhipādā …pe…",
  "kv21.1:3.7_0": "indriyā …pe…",
  "kv21.1:3.8_0": "balā …pe…",
  "kv21.1:3.9_0": "bojjhaṅgā puna navaṁ kātunti?",
  "kv21.1:3.10_0": "Na hevaṁ vattabbe …pe…",
  "kv21.1:3.11_0": "labbhā pubbe akusalaṁ pacchā kusalaṁ kātunti?",
  "kv21.1:3.12_0": "Na hevaṁ vattabbe …pe…",
  "kv21.1:3.13_0": "labbhā pubbe sāsavaṁ …pe… saṅkilesiyaṁ pacchā asaṅkilesiyaṁ kātunti?",
  "kv21.1:3.14_0": "Na hevaṁ vattabbe …pe….",
  "kv21.1:3.15_0": "Sāsanakathā niṭṭhitā."
}