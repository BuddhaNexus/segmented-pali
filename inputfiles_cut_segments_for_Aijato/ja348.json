{
  "ja348:0.1_0": "Jātaka",
  "ja348:0.2_0": "Catukkanipāta",
  "ja348:0.3_0": "Cūḷakuṇālavagga",
  "ja348:0.4_0": "8. Araññajātaka",
  "ja348:1.1_0": "“Araññā gāmamāgamma,",
  "ja348:1.2_0": "kiṁsīlaṁ kiṁvataṁ ahaṁ;",
  "ja348:1.3_0": "Purisaṁ tāta seveyyaṁ,",
  "ja348:1.4_0": "taṁ me akkhāhi pucchito”.",
  "ja348:2.1_0": "“Yo taṁ vissāsaye tāta,",
  "ja348:2.2_0": "vissāsañca khameyya te;",
  "ja348:2.3_0": "Sussūsī ca titikkhī ca,",
  "ja348:2.4_0": "taṁ bhajehi ito gato.",
  "ja348:3.1_0": "Yassa kāyena vācāya,",
  "ja348:3.2_0": "manasā natthi dukkaṭaṁ;",
  "ja348:3.3_0": "Urasīva patiṭṭhāya,",
  "ja348:3.4_0": "taṁ bhajehi ito gato.",
  "ja348:4.1_0": "Haliddirāgaṁ kapicittaṁ,",
  "ja348:4.2_0": "purisaṁ rāgavirāginaṁ;",
  "ja348:4.3_0": "Tādisaṁ tāta mā sevi,",
  "ja348:4.4_0": "nimmanussampi ce siyā”ti.",
  "ja348:5.1_0": "Araññajātakaṁ aṭṭhamaṁ."
}