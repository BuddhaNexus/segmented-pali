{
  "ja61:0.1_0": "Jātaka",
  "ja61:0.2_0": "Ekakanipāta",
  "ja61:0.3_0": "Itthivagga",
  "ja61:0.4_0": "1. Asātamantajātaka",
  "ja61:1.1_0": "“Asā lokitthiyo nāma,",
  "ja61:1.2_0": "velā tāsaṁ na vijjati;",
  "ja61:1.3_0": "Sārattā ca pagabbhā ca,",
  "ja61:1.4_0": "sikhī sabbaghaso yathā;",
  "ja61:1.5_0": "Tā hitvā pabbajissāmi,",
  "ja61:1.6_0": "vivekamanubrūhayan”ti.",
  "ja61:2.1_0": "Asātamantajātakaṁ paṭhamaṁ."
}