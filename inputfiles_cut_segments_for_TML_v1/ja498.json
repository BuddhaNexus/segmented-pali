{
  "ja498:0.1_0": "Jātaka",
  "ja498:0.2_0": "Vīsatinipāta",
  "ja498:0.3_0": "Mātaṅgavagga",
  "ja498:0.4_0": "2. Cittasambhūtajātaka",
  "ja498:1.1_0": "“Sabbaṁ narānaṁ saphalaṁ suciṇṇaṁ,",
  "ja498:1.2_0": "Na kammunā kiñcana moghamatthi;",
  "ja498:1.3_0": "Passāmi sambhūtaṁ mahānubhāvaṁ,",
  "ja498:1.4_0": "Sakammunā puññaphalūpapannaṁ.",
  "ja498:2.1_0": "Sabbaṁ narānaṁ saphalaṁ suciṇṇaṁ,",
  "ja498:2.2_0": "Na kammunā kiñcana moghamatthi;",
  "ja498:2.3_0": "Kaccinnu cittassapi evamevaṁ,",
  "ja498:2.4_0": "Iddho mano tassa yathāpi mayhaṁ”.",
  "ja498:3.1_0": "“Sabbaṁ narānaṁ saphalaṁ suciṇṇaṁ,",
  "ja498:3.2_0": "Na kammunā kiñcana moghamatthi;",
  "ja498:3.3_0": "Cittampi jānāhi tatheva deva,",
  "ja498:3.4_0": "Iddho mano tassa yathāpi tuyhaṁ”.",
  "ja498:4.1_0": "“Bhavaṁ nu citto sutamaññato te,",
  "ja498:4.2_0": "Udāhu te koci naṁ etadakkhā;",
  "ja498:4.3_0": "Gāthā sugītā na mamatthi kaṅkhā,",
  "ja498:4.4_0": "Dadāmi te gāmavaraṁ satañca”.",
  "ja498:5.1_0": "“Na cāhaṁ citto sutamaññato me,",
  "ja498:5.2_0": "Isī ca me etamatthaṁ asaṁsi;",
  "ja498:5.3_0": "‘Gantvāna rañño paṭigāhi gāthaṁ,",
  "ja498:5.4_0": "Api te varaṁ attamano dadeyya’”.",
  "ja498:6.1_0": "“Yojentu ve rājarathe,",
  "ja498:6.2_0": "sukate cittasibbane;",
  "ja498:6.3_0": "Kacchaṁ nāgānaṁ bandhatha,",
  "ja498:6.4_0": "gīveyyaṁ paṭimuñcatha.",
  "ja498:7.1_0": "Āhaññantu bherimudiṅgasaṅkhe,",
  "ja498:7.2_0": "Sīghāni yānāni ca yojayantu;",
  "ja498:7.3_0": "Ajjevahaṁ assamaṁ taṁ gamissaṁ,",
  "ja498:7.4_0": "Yattheva dakkhissamisiṁ nisinnaṁ”.",
  "ja498:8.1_0": "“Suladdhalābho vata me ahosi,",
  "ja498:8.2_0": "Gāthā sugītā parisāya majjhe;",
  "ja498:8.3_0": "Svāhaṁ isiṁ sīlavatūpapannaṁ,",
  "ja498:8.4_0": "Disvā patīto sumanohamasmi”.",
  "ja498:9.1_0": "“Āsanaṁ udakaṁ pajjaṁ,",
  "ja498:9.2_0": "paṭiggaṇhātu no bhavaṁ;",
  "ja498:9.3_0": "Agghe bhavantaṁ pucchāma,",
  "ja498:9.4_0": "agghaṁ kurutu no bhavaṁ”.",
  "ja498:10.1_0": "“Rammañca te āvasathaṁ karontu,",
  "ja498:10.2_0": "Nārīgaṇehi paricārayassu;",
  "ja498:10.3_0": "Karohi okāsamanuggahāya,",
  "ja498:10.4_0": "Ubhopimaṁ issariyaṁ karoma”.",
  "ja498:11.1_0": "“Disvā phalaṁ duccaritassa rāja,",
  "ja498:11.2_0": "Atho suciṇṇassa mahāvipākaṁ;",
  "ja498:11.3_0": "Attānameva paṭisaṁyamissaṁ,",
  "ja498:11.4_0": "Na patthaye putta pasuṁ dhanaṁ vā.",
  "ja498:12.1_0": "Dasevimā vassadasā,",
  "ja498:12.2_0": "maccānaṁ idha jīvitaṁ;",
  "ja498:12.3_0": "Apattaññeva taṁ odhiṁ,",
  "ja498:12.4_0": "naḷo chinnova sussati.",
  "ja498:13.1_0": "Tattha kā nandi kā khiḍḍā,",
  "ja498:13.2_0": "kā ratī kā dhanesanā;",
  "ja498:13.3_0": "Kiṁ me puttehi dārehi,",
  "ja498:13.4_0": "rāja muttosmi bandhanā.",
  "ja498:14.1_0": "Sohaṁ evaṁ pajānāmi,",
  "ja498:14.2_0": "maccu me nappamajjati;",
  "ja498:14.3_0": "Antakenādhipannassa,",
  "ja498:14.4_0": "kā ratī kā dhanesanā.",
  "ja498:15.1_0": "Jāti narānaṁ adhamā janinda,",
  "ja498:15.2_0": "Caṇḍālayoni dvipadākaniṭṭhā;",
  "ja498:15.3_0": "Sakehi kammehi supāpakehi,",
  "ja498:15.4_0": "Caṇḍālagabbhe avasimha pubbe.",
  "ja498:16.1_0": "Caṇḍālāhumha avantīsu,",
  "ja498:16.2_0": "migā nerañjaraṁ pati;",
  "ja498:16.3_0": "Ukkusā nammadātīre,",
  "ja498:16.4_0": "tyajja brāhmaṇakhattiyā.",
  "ja498:17.1_0": "Upanīyati jīvitamappamāyu,",
  "ja498:17.2_0": "Jarūpanītassa na santi tāṇā;",
  "ja498:17.3_0": "Karohi pañcāla mameta vākyaṁ,",
  "ja498:17.4_0": "Mākāsi kammāni dukkhudrayāni.",
  "ja498:18.1_0": "Upanīyati jīvitamappamāyu,",
  "ja498:18.2_0": "Jarūpanītassa na santi tāṇā;",
  "ja498:18.3_0": "Karohi pañcāla mameta vākyaṁ,",
  "ja498:18.4_0": "Mākāsi kammāni dukkhapphalāni.",
  "ja498:19.1_0": "Upanīyati jīvitamappamāyu,",
  "ja498:19.2_0": "Jarūpanītassa na santi tāṇā;",
  "ja498:19.3_0": "Karohi pañcāla mameta vākyaṁ,",
  "ja498:19.4_0": "Mākāsi kammāni rajassirāni.",
  "ja498:20.1_0": "Upanīyati jīvitamappamāyu,",
  "ja498:20.2_0": "Vaṇṇaṁ jarā hanti narassa jiyyato;",
  "ja498:20.3_0": "Karohi pañcāla mameta vākyaṁ,",
  "ja498:20.4_0": "Mākāsi kammaṁ nirayūpapattiyā”.",
  "ja498:21.1_0": "“Addhā hi saccaṁ vacanaṁ tavetaṁ,",
  "ja498:21.2_0": "Yathā isī bhāsasi evametaṁ;",
  "ja498:21.3_0": "Kāmā ca me santi anapparūpā,",
  "ja498:21.4_0": "Te duccajā mādisakena bhikkhu.",
  "ja498:22.1_0": "Nāgo yathā paṅkamajjhe byasanno,",
  "ja498:22.2_0": "Passaṁ thalaṁ nābhisambhoti gantuṁ;",
  "ja498:22.3_0": "Evampahaṁ kāmapaṅke byasanno,",
  "ja498:22.4_0": "Na bhikkhuno maggamanubbajāmi.",
  "ja498:23.1_0": "Yathāpi mātā ca pitā ca puttaṁ,",
  "ja498:23.2_0": "Anusāsare kinti sukhī bhaveyya;",
  "ja498:23.3_0": "Evampi maṁ tvaṁ anusāsa bhante,",
  "ja498:23.4_0": "Yathā ciraṁ pecca sukhī bhaveyyaṁ”.",
  "ja498:24.1_0": "“No ce tuvaṁ ussahase janinda,",
  "ja498:24.2_0": "Kāme ime mānusake pahātuṁ;",
  "ja498:24.3_0": "Dhammiṁ baliṁ paṭṭhapayassu rāja,",
  "ja498:24.4_0": "Adhammakāro tava māhu raṭṭhe.",
  "ja498:25.1_0": "Dūtā vidhāvantu disā catasso,",
  "ja498:25.2_0": "Nimantakā samaṇabrāhmaṇānaṁ;",
  "ja498:25.3_0": "Te annapānena upaṭṭhahassu,",
  "ja498:25.4_0": "Vatthena senāsanapaccayena ca.",
  "ja498:26.1_0": "Annena pānena pasannacitto,",
  "ja498:26.2_0": "Santappaya samaṇabrāhmaṇe ca;",
  "ja498:26.3_0": "Datvā ca bhutvā ca yathānubhāvaṁ,",
  "ja498:26.4_0": "Anindito saggamupehi ṭhānaṁ.",
  "ja498:27.1_0": "Sace ca taṁ rāja mado saheyya,",
  "ja498:27.2_0": "Nārīgaṇehi paricārayantaṁ;",
  "ja498:27.3_0": "Imameva gāthaṁ manasī karohi,",
  "ja498:27.4_0": "Bhāsesi cenaṁ parisāya majjhe.",
  "ja498:28.1_0": "Abbhokāsasayo jantu,",
  "ja498:28.2_0": "vajantyā khīrapāyito;",
  "ja498:28.3_0": "Parikiṇṇo suvānehi,",
  "ja498:28.4_0": "svājja rājāti vuccatī”ti.",
  "ja498:29.1_0": "Cittasambhūtajātakaṁ dutiyaṁ."
}