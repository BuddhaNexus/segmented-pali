{
  "tha-ap273:0.1_0": "Therāpadāna",
  "tha-ap273:0.2_0": "Suvaṇṇabibbohanavagga",
  "tha-ap273:0.3_0": "1 Suvaṇṇabibbohaniyattheraapadāna",
  "tha-ap273:1.1_0": "“Ekāsanaṁ ahamadaṁ,",
  "tha-ap273:1.2_0": "pasanno sehi pāṇibhi;",
  "tha-ap273:1.3_0": "Bibbohanañca pādāsiṁ,",
  "tha-ap273:1.4_0": "uttamatthassa pattiyā.",
  "tha-ap273:2.1_0": "Ekanavutito kappe,",
  "tha-ap273:2.2_0": "bibbohanamadāsahaṁ;",
  "tha-ap273:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap273:2.4_0": "bibbohanassidaṁ phalaṁ.",
  "tha-ap273:3.1_0": "Ito tesaṭṭhime kappe,",
  "tha-ap273:3.2_0": "asamo nāma khattiyo;",
  "tha-ap273:3.3_0": "Sattaratanasampanno,",
  "tha-ap273:3.4_0": "cakkavattī mahabbalo.",
  "tha-ap273:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap273:4.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap273:4.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap273:4.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap273:5.1_0": "Itthaṁ sudaṁ āyasmā suvaṇṇabibbohaniyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap273:6.1_0": "Suvaṇṇabibbohaniyattherassāpadānaṁ paṭhamaṁ."
}