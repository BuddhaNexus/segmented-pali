{
  "thig1.17:0.1_0": "Therīgāthā",
  "thig1.17:0.2_0": "Ekakanipāta",
  "thig1.17:0.3_0": "17. Dhammātherīgāthā",
  "thig1.17:1.1_0": "“Piṇḍapātaṁ caritvāna,",
  "thig1.17:1.2_0": "daṇḍamolubbha dubbalā;",
  "thig1.17:1.3_0": "Vedhamānehi gattehi,",
  "thig1.17:1.4_0": "tattheva nipatiṁ chamā;",
  "thig1.17:1.5_0": "Disvā ādīnavaṁ kāye,",
  "thig1.17:1.6_0": "atha cittaṁ vimucci me”ti.",
  "thig1.17:2.1_0": "… Dhammā therī …."
}