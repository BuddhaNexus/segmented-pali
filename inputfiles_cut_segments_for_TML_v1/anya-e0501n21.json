{
  "anya-e0501n21:0_0": "22. Natthipaccayo",
  "anya-e0501n21:1_0": "23. Vigatapaccayo",
  "anya-e0501n21:2_0": "24. Avigatapaccayo",
  "anya-e0501n21:3_0": "Sabbo anantarapaccayo natthi paccayo nāma.",
  "anya-e0501n21:3_1": "Tathā vigata paccayo.",
  "anya-e0501n21:3_2": "Avigatapaccayopi atthipaccayena sabbasadiso.",
  "anya-e0501n21:3_3": "Atthītica avigatoti ca atthato ekameva.",
  "anya-e0501n21:3_4": "Tathā natthīti ca vigatotica.",
  "anya-e0501n21:3_5": "Natthivigataavigatapaccayadīpanā niṭṭhitā.",
  "anya-e0501n21:4_0": "Paccayatthadīpanā niṭṭhitā."
}