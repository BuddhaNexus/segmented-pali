{
  "ja375:0.1_0": "Jātaka",
  "ja375:0.2_0": "Pañcakanipāta",
  "ja375:0.3_0": "Aḍḍhavagga",
  "ja375:0.4_0": "5. Kapotajātaka",
  "ja375:1.1_0": "“Idāni khomhi sukhito arogo,",
  "ja375:1.2_0": "Nikkaṇṭako nippatito kapoto;",
  "ja375:1.3_0": "Kāhāmi dānī hadayassa tuṭṭhiṁ,",
  "ja375:1.4_0": "Tathāhimaṁ maṁsasākaṁ baleti”.",
  "ja375:2.1_0": "“Kāyaṁ balākā sikhinī,",
  "ja375:2.2_0": "corī laṅghipitāmahā;",
  "ja375:2.3_0": "Oraṁ balāke āgaccha,",
  "ja375:2.4_0": "caṇḍo me vāyaso sakhā”.",
  "ja375:3.1_0": "“Alañhi te jagghitāye,",
  "ja375:3.2_0": "Mamaṁ disvāna edisaṁ;",
  "ja375:3.3_0": "Vilūnaṁ sūdaputtena,",
  "ja375:3.4_0": "Piṭṭhamaṇḍena makkhitaṁ”.",
  "ja375:4.1_0": "“Sunhāto suvilittosi,",
  "ja375:4.2_0": "annapānena tappito;",
  "ja375:4.3_0": "Kaṇṭhe ca te veḷuriyo,",
  "ja375:4.4_0": "agamā nu kajaṅgalaṁ”.",
  "ja375:5.1_0": "“Mā te mitto amitto vā,",
  "ja375:5.2_0": "agamāsi kajaṅgalaṁ;",
  "ja375:5.3_0": "Piñchāni tattha lāyitvā,",
  "ja375:5.4_0": "kaṇṭhe bandhanti vaṭṭanaṁ”.",
  "ja375:6.1_0": "“Punapāpajjasī samma,",
  "ja375:6.2_0": "Sīlañhi tava tādisaṁ;",
  "ja375:6.3_0": "Na hi mānusakā bhogā,",
  "ja375:6.4_0": "Subhuñjā honti pakkhinā”ti. ",
  "ja375:7.1_0": "Kapotajātakaṁ pañcamaṁ.",
  "ja375:7.2_0": "Aḍḍhavaggo tatiyo.",
  "ja375:8.1_0": "Tassuddānaṁ",
  "ja375:9.1_0": "Atha vaṇṇa sasīla hiri labhate,",
  "ja375:9.2_0": "Sumukhā visa sāḷiyamittavaro;",
  "ja375:9.3_0": "Atha cakka palāsa sarāja sato,",
  "ja375:9.4_0": "Yava bāla kapotaka pannarasāti.",
  "ja375:10.1_0": "Atha vagguddānaṁ",
  "ja375:11.1_0": "Jīnañca vaṇṇaṁ asamaṁvaguppari,",
  "ja375:11.2_0": "Sudesitā jātakanti santi vīsati;",
  "ja375:11.3_0": "Mahesino brahmacarittamutta-",
  "ja375:11.4_0": "Mavoca gāthā atthavatī subyañjanāti.",
  "ja375:12.1_0": "Pañcakanipātaṁ niṭṭhitaṁ."
}