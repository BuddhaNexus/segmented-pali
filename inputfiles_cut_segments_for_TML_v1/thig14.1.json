{
  "thig14.1:0.1_0": "Therīgāthā",
  "thig14.1:0.2_0": "Tiṁsanipāta",
  "thig14.1:0.3_0": "1. Subhājīvakambavanikātherīgāthā",
  "thig14.1:1.1_0": "Jīvakambavanaṁ rammaṁ,",
  "thig14.1:1.2_0": "Gacchantiṁ bhikkhuniṁ subhaṁ;",
  "thig14.1:1.3_0": "Dhuttako sannivāresi,",
  "thig14.1:1.4_0": "Tamenaṁ abravī subhā.",
  "thig14.1:2.1_0": "“Kiṁ te aparādhitaṁ mayā,",
  "thig14.1:2.2_0": "Yaṁ maṁ ovariyāna tiṭṭhasi;",
  "thig14.1:2.3_0": "Na hi pabbajitāya āvuso,",
  "thig14.1:2.4_0": "Puriso samphusanāya kappati.",
  "thig14.1:3.1_0": "Garuke mama satthusāsane,",
  "thig14.1:3.2_0": "Yā sikkhā sugatena desitā;",
  "thig14.1:3.3_0": "Parisuddhapadaṁ anaṅgaṇaṁ,",
  "thig14.1:3.4_0": "Kiṁ maṁ ovariyāna tiṭṭhasi.",
  "thig14.1:4.1_0": "Āvilacitto anāvilaṁ,",
  "thig14.1:4.2_0": "Sarajo vītarajaṁ anaṅgaṇaṁ;",
  "thig14.1:4.3_0": "Sabbattha vimuttamānasaṁ,",
  "thig14.1:4.4_0": "Kiṁ maṁ ovariyāna tiṭṭhasi”.",
  "thig14.1:5.1_0": "“Daharā ca apāpikā casi,",
  "thig14.1:5.2_0": "Kiṁ te pabbajjā karissati;",
  "thig14.1:5.3_0": "Nikkhipa kāsāyacīvaraṁ,",
  "thig14.1:5.4_0": "Ehi ramāma supupphite vane.",
  "thig14.1:6.1_0": "Madhurañca pavanti sabbaso,",
  "thig14.1:6.2_0": "Kusumarajena samuṭṭhitā dumā;",
  "thig14.1:6.3_0": "Paṭhamavasanto sukho utu,",
  "thig14.1:6.4_0": "Ehi ramāma supupphite vane.",
  "thig14.1:7.1_0": "Kusumitasikharā ca pādapā,",
  "thig14.1:7.2_0": "Abhigajjantiva māluteritā;",
  "thig14.1:7.3_0": "Kā tuyhaṁ rati bhavissati,",
  "thig14.1:7.4_0": "Yadi ekā vanamogahissasi.",
  "thig14.1:8.1_0": "Vāḷamigasaṅghasevitaṁ,",
  "thig14.1:8.2_0": "Kuñjaramattakareṇuloḷitaṁ;",
  "thig14.1:8.3_0": "Asahāyikā gantumicchasi,",
  "thig14.1:8.4_0": "Rahitaṁ bhiṁsanakaṁ mahāvanaṁ.",
  "thig14.1:9.1_0": "Tapanīyakatāva dhītikā,",
  "thig14.1:9.2_0": "Vicarasi cittalateva accharā;",
  "thig14.1:9.3_0": "Kāsikasukhumehi vaggubhi,",
  "thig14.1:9.4_0": "Sobhasī suvasanehi nūpame.",
  "thig14.1:10.1_0": "Ahaṁ tava vasānugo siyaṁ,",
  "thig14.1:10.2_0": "Yadi viharemase kānanantare;",
  "thig14.1:10.3_0": "Na hi matthi tayā piyattaro,",
  "thig14.1:10.4_0": "Pāṇo kinnarimandalocane.",
  "thig14.1:11.1_0": "Yadi me vacanaṁ karissasi,",
  "thig14.1:11.2_0": "Sukhitā ehi agāramāvasa;",
  "thig14.1:11.3_0": "Pāsādanivātavāsinī,",
  "thig14.1:11.4_0": "Parikammaṁ te karontu nāriyo.",
  "thig14.1:12.1_0": "Kāsikasukhumāni dhāraya,",
  "thig14.1:12.2_0": "Abhiropehi ca mālavaṇṇakaṁ;",
  "thig14.1:12.3_0": "Kañcanamaṇimuttakaṁ bahuṁ,",
  "thig14.1:12.4_0": "Vividhaṁ ābharaṇaṁ karomi te.",
  "thig14.1:13.1_0": "Sudhotarajapacchadaṁ subhaṁ,",
  "thig14.1:13.2_0": "Gonakatūlikasanthataṁ navaṁ;",
  "thig14.1:13.3_0": "Abhiruha sayanaṁ mahārahaṁ,",
  "thig14.1:13.4_0": "Candanamaṇḍitasāragandhikaṁ.",
  "thig14.1:14.1_0": "Uppalaṁ cudakā samuggataṁ,",
  "thig14.1:14.2_0": "Yathā taṁ amanussasevitaṁ;",
  "thig14.1:14.3_0": "Evaṁ tvaṁ brahmacārinī,",
  "thig14.1:14.4_0": "Sakesaṅgesu jaraṁ gamissasi”.",
  "thig14.1:15.1_0": "“Kiṁ te idha sārasammataṁ,",
  "thig14.1:15.2_0": "Kuṇapapūramhi susānavaḍḍhane;",
  "thig14.1:15.3_0": "Bhedanadhamme kaḷevare,",
  "thig14.1:15.4_0": "Yaṁ disvā vimano udikkhasi”.",
  "thig14.1:16.1_0": "“Akkhīni ca tūriyāriva,",
  "thig14.1:16.2_0": "Kinnariyāriva pabbatantare;",
  "thig14.1:16.3_0": "Tava me nayanāni dakkhiya,",
  "thig14.1:16.4_0": "Bhiyyo kāmaratī pavaḍḍhati.",
  "thig14.1:17.1_0": "Uppalasikharopamāni te,",
  "thig14.1:17.2_0": "Vimale hāṭakasannibhe mukhe;",
  "thig14.1:17.3_0": "Tava me nayanāni dakkhiya,",
  "thig14.1:17.4_0": "Bhiyyo kāmaguṇo pavaḍḍhati.",
  "thig14.1:18.1_0": "Api dūragatā saramhase,",
  "thig14.1:18.2_0": "Āyatapamhe visuddhadassane;",
  "thig14.1:18.3_0": "Na hi matthi tayā piyattaro,",
  "thig14.1:18.4_0": "Nayanā kinnarimandalocane”.",
  "thig14.1:19.1_0": "“Apathena payātumicchasi,",
  "thig14.1:19.2_0": "Candaṁ kīḷanakaṁ gavesasi;",
  "thig14.1:19.3_0": "Meruṁ laṅghetumicchasi,",
  "thig14.1:19.4_0": "Yo tvaṁ buddhasutaṁ maggayasi.",
  "thig14.1:20.1_0": "Natthi hi loke sadevake,",
  "thig14.1:20.2_0": "Rāgo yatthapi dāni me siyā;",
  "thig14.1:20.3_0": "Napi naṁ jānāmi kīriso,",
  "thig14.1:20.4_0": "Atha maggena hato samūlako.",
  "thig14.1:21.1_0": "Iṅgālakuyāva ujjhito,",
  "thig14.1:21.2_0": "Visapattoriva aggito kato;",
  "thig14.1:21.3_0": "Napi naṁ passāmi kīriso,",
  "thig14.1:21.4_0": "Atha maggena hato samūlako.",
  "thig14.1:22.1_0": "Yassā siyā apaccavekkhitaṁ,",
  "thig14.1:22.2_0": "Satthā vā anupāsito siyā;",
  "thig14.1:22.3_0": "Tvaṁ tādisikaṁ palobhaya,",
  "thig14.1:22.4_0": "Jānantiṁ so imaṁ vihaññasi.",
  "thig14.1:23.1_0": "Mayhañhi akkuṭṭhavandite,",
  "thig14.1:23.2_0": "Sukhadukkhe ca satī upaṭṭhitā;",
  "thig14.1:23.3_0": "Saṅkhatamasubhanti jāniya,",
  "thig14.1:23.4_0": "Sabbattheva mano na limpati.",
  "thig14.1:24.1_0": "Sāhaṁ sugatassa sāvikā,",
  "thig14.1:24.2_0": "Maggaṭṭhaṅgikayānayāyinī;",
  "thig14.1:24.3_0": "Uddhaṭasallā anāsavā,",
  "thig14.1:24.4_0": "Suññāgāragatā ramāmahaṁ.",
  "thig14.1:25.1_0": "Diṭṭhā hi mayā sucittitā,",
  "thig14.1:25.2_0": "Sombhā dārukapillakāni vā;",
  "thig14.1:25.3_0": "Tantīhi ca khīlakehi ca,",
  "thig14.1:25.4_0": "Vinibaddhā vividhaṁ panaccakā.",
  "thig14.1:26.1_0": "Tamhuddhaṭe tantikhīlake,",
  "thig14.1:26.2_0": "Vissaṭṭhe vikale parikrite;",
  "thig14.1:26.3_0": "Na vindeyya khaṇḍaso kate,",
  "thig14.1:26.4_0": "Kimhi tattha manaṁ nivesaye.",
  "thig14.1:27.1_0": "Tathūpamā dehakāni maṁ,",
  "thig14.1:27.2_0": "Tehi dhammehi vinā na vattanti;",
  "thig14.1:27.3_0": "Dhammehi vinā na vattati,",
  "thig14.1:27.4_0": "Kimhi tattha manaṁ nivesaye.",
  "thig14.1:28.1_0": "Yathā haritālena makkhitaṁ,",
  "thig14.1:28.2_0": "Addasa cittikaṁ bhittiyā kataṁ;",
  "thig14.1:28.3_0": "Tamhi te viparītadassanaṁ,",
  "thig14.1:28.4_0": "Saññā mānusikā niratthikā.",
  "thig14.1:29.1_0": "Māyaṁ viya aggato kataṁ,",
  "thig14.1:29.2_0": "Supinanteva suvaṇṇapādapaṁ;",
  "thig14.1:29.3_0": "Upagacchasi andha rittakaṁ,",
  "thig14.1:29.4_0": "Janamajjheriva rupparūpakaṁ.",
  "thig14.1:30.1_0": "Vaṭṭaniriva koṭarohitā,",
  "thig14.1:30.2_0": "Majjhe pubbuḷakā saassukā;",
  "thig14.1:30.3_0": "Pīḷakoḷikā cettha jāyati,",
  "thig14.1:30.4_0": "Vividhā cakkhuvidhā ca piṇḍitā”.",
  "thig14.1:31.1_0": "Uppāṭiya cārudassanā,",
  "thig14.1:31.2_0": "Na ca pajjittha asaṅgamānasā;",
  "thig14.1:31.3_0": "“Handa te cakkhuṁ harassu taṁ”,",
  "thig14.1:31.4_0": "Tassa narassa adāsi tāvade.",
  "thig14.1:32.1_0": "Tassa ca viramāsi tāvade,",
  "thig14.1:32.2_0": "Rāgo tattha khamāpayī ca naṁ;",
  "thig14.1:32.3_0": "“Sotthi siyā brahmacārinī,",
  "thig14.1:32.4_0": "Na puno edisakaṁ bhavissati”.",
  "thig14.1:33.1_0": "“Āsādiya edisaṁ janaṁ,",
  "thig14.1:33.2_0": "Aggiṁ pajjalitaṁva liṅgiya;",
  "thig14.1:33.3_0": "Gaṇhiya āsīvisaṁ viya,",
  "thig14.1:33.4_0": "Api nu sotthi siyā khamehi no”.",
  "thig14.1:34.1_0": "Muttā ca tato sā bhikkhunī,",
  "thig14.1:34.2_0": "Agamī buddhavarassa santikaṁ;",
  "thig14.1:34.3_0": "Passiya varapuññalakkhaṇaṁ,",
  "thig14.1:34.4_0": "Cakkhu āsi yathā purāṇakanti.",
  "thig14.1:35.1_0": "…",
  "thig14.1:35.2_0": "Subhā jīvakambavanikā therī ….",
  "thig14.1:36.1_0": "Tiṁsanipāto niṭṭhito."
}