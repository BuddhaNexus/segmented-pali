{
  "ja199:0.1_0": "Jātaka",
  "ja199:0.2_0": "Dukanipāta",
  "ja199:0.3_0": "Ruhakavagga",
  "ja199:0.4_0": "9. Gahapatijātaka",
  "ja199:1.1_0": "“Ubhayaṁ me na khamati,",
  "ja199:1.2_0": "ubhayaṁ me na ruccati;",
  "ja199:1.3_0": "Yācāyaṁ koṭṭhamotiṇṇā,",
  "ja199:1.4_0": "nāddasaṁ iti bhāsati.",
  "ja199:2.1_0": "Taṁ taṁ gāmapati brūmi,",
  "ja199:2.2_0": "kadare appasmi jīvite;",
  "ja199:2.3_0": "Dve māse saṅgaraṁ katvā,",
  "ja199:2.4_0": "maṁsaṁ jaraggavaṁ kisaṁ;",
  "ja199:2.5_0": "Appattakāle codesi,",
  "ja199:2.6_0": "tampi mayhaṁ na ruccatī”ti.",
  "ja199:3.1_0": "Gahapatijātakaṁ navamaṁ."
}