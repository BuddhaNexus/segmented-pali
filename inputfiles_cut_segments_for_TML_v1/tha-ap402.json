{
  "tha-ap402:0.1_0": "Therāpadāna",
  "tha-ap402:0.2_0": "Pilindavacchavagga",
  "tha-ap402:0.3_0": "10. Ajitattheraapadāna",
  "tha-ap402:1.1_0": "“Padumuttaro nāma jino,",
  "tha-ap402:1.2_0": "sabbadhammāna pāragū;",
  "tha-ap402:1.3_0": "Ajjhogāhetvā himavantaṁ,",
  "tha-ap402:1.4_0": "nisīdi lokanāyako.",
  "tha-ap402:2.1_0": "Nāhaṁ addakkhiṁ sambuddhaṁ,",
  "tha-ap402:2.2_0": "Napi saddaṁ suṇomahaṁ;",
  "tha-ap402:2.3_0": "Mama bhakkhaṁ gavesanto,",
  "tha-ap402:2.4_0": "Āhiṇḍāmi vane ahaṁ.",
  "tha-ap402:3.1_0": "Tatthaddassāsiṁ sambuddhaṁ,",
  "tha-ap402:3.2_0": "dvattiṁsavaralakkhaṇaṁ;",
  "tha-ap402:3.3_0": "Disvāna vittimāpajjiṁ,",
  "tha-ap402:3.4_0": "satto ko nāmayaṁ bhave.",
  "tha-ap402:4.1_0": "Lakkhaṇāni viloketvā,",
  "tha-ap402:4.2_0": "mama vijjaṁ anussariṁ;",
  "tha-ap402:4.3_0": "Sutañhi metaṁ vuḍḍhānaṁ,",
  "tha-ap402:4.4_0": "paṇḍitānaṁ subhāsitaṁ.",
  "tha-ap402:5.1_0": "Tesaṁ yathā taṁ vacanaṁ,",
  "tha-ap402:5.2_0": "ayaṁ buddho bhavissati;",
  "tha-ap402:5.3_0": "Yannūnāhaṁ sakkareyyaṁ,",
  "tha-ap402:5.4_0": "gatiṁ me sodhayissati.",
  "tha-ap402:6.1_0": "Khippaṁ assamamāgantvā,",
  "tha-ap402:6.2_0": "madhutelaṁ gahiṁ ahaṁ;",
  "tha-ap402:6.3_0": "Kolambakaṁ gahetvāna,",
  "tha-ap402:6.4_0": "upagacchiṁ vināyakaṁ.",
  "tha-ap402:7.1_0": "Tidaṇḍake gahetvāna,",
  "tha-ap402:7.2_0": "abbhokāse ṭhapesahaṁ;",
  "tha-ap402:7.3_0": "Padīpaṁ pajjalitvāna,",
  "tha-ap402:7.4_0": "aṭṭhakkhattuṁ avandahaṁ.",
  "tha-ap402:8.1_0": "Sattarattindivaṁ buddho,",
  "tha-ap402:8.2_0": "nisīdi purisuttamo;",
  "tha-ap402:8.3_0": "Tato ratyā vivasāne,",
  "tha-ap402:8.4_0": "vuṭṭhāsi lokanāyako.",
  "tha-ap402:9.1_0": "Pasannacitto sumano,",
  "tha-ap402:9.2_0": "sabbarattindivaṁ ahaṁ;",
  "tha-ap402:9.3_0": "Dīpaṁ buddhassa pādāsiṁ,",
  "tha-ap402:9.4_0": "pasanno sehi pāṇibhi.",
  "tha-ap402:10.1_0": "Sabbe vanā gandhamayā,",
  "tha-ap402:10.2_0": "Pabbate gandhamādane;",
  "tha-ap402:10.3_0": "Buddhassa ānubhāvena,",
  "tha-ap402:10.4_0": "Āgacchuṁ buddhasantikaṁ.",
  "tha-ap402:11.1_0": "Ye keci pupphagandhāse,",
  "tha-ap402:11.2_0": "pupphitā dharaṇīruhā;",
  "tha-ap402:11.3_0": "Buddhassa ānubhāvena,",
  "tha-ap402:11.4_0": "sabbe sannipatuṁ tadā.",
  "tha-ap402:12.1_0": "Yāvatā himavantamhi,",
  "tha-ap402:12.2_0": "nāgā ca garuḷā ubho;",
  "tha-ap402:12.3_0": "Dhammañca sotukāmā te,",
  "tha-ap402:12.4_0": "āgacchuṁ buddhasantikaṁ.",
  "tha-ap402:13.1_0": "Devalo nāma samaṇo,",
  "tha-ap402:13.2_0": "buddhassa aggasāvako;",
  "tha-ap402:13.3_0": "Vasīsatasahassehi,",
  "tha-ap402:13.4_0": "buddhasantikupāgami.",
  "tha-ap402:14.1_0": "Padumuttaro lokavidū,",
  "tha-ap402:14.2_0": "āhutīnaṁ paṭiggaho;",
  "tha-ap402:14.3_0": "Bhikkhusaṅghe nisīditvā,",
  "tha-ap402:14.4_0": "imā gāthā abhāsatha.",
  "tha-ap402:15.1_0": "‘Yo me dīpaṁ padīpesi,",
  "tha-ap402:15.2_0": "pasanno sehi pāṇibhi;",
  "tha-ap402:15.3_0": "Tamahaṁ kittayissāmi,",
  "tha-ap402:15.4_0": "suṇātha mama bhāsato.",
  "tha-ap402:16.1_0": "Saṭṭhi kappasahassāni,",
  "tha-ap402:16.2_0": "devaloke ramissati;",
  "tha-ap402:16.3_0": "Sahassakkhattuṁ rājā ca,",
  "tha-ap402:16.4_0": "cakkavattī bhavissati.",
  "tha-ap402:17.1_0": "Soḷasamaṁ bhāṇavāraṁ.",
  "tha-ap402:18.1_0": "Chattiṁsakkhattuṁ devindo,",
  "tha-ap402:18.2_0": "devarajjaṁ karissati;",
  "tha-ap402:18.3_0": "Pathaviyaṁ sattasataṁ,",
  "tha-ap402:18.4_0": "vipulaṁ rajjaṁ karissati.",
  "tha-ap402:19.1_0": "Padesarajjaṁ vipulaṁ,",
  "tha-ap402:19.2_0": "gaṇanāto asaṅkhiyaṁ;",
  "tha-ap402:19.3_0": "Iminā dīpadānena,",
  "tha-ap402:19.4_0": "dibbacakkhu bhavissati.",
  "tha-ap402:20.1_0": "Samantato aṭṭhakosaṁ,",
  "tha-ap402:20.2_0": "passissati ayaṁ sadā;",
  "tha-ap402:20.3_0": "Devalokā cavantassa,",
  "tha-ap402:20.4_0": "nibbattantassa jantuno.",
  "tha-ap402:21.1_0": "Divā vā yadi vā rattiṁ,",
  "tha-ap402:21.2_0": "padīpaṁ dhārayissati;",
  "tha-ap402:21.3_0": "Jāyamānassa sattassa,",
  "tha-ap402:21.4_0": "puññakammasamaṅgino.",
  "tha-ap402:22.1_0": "Yāvatā nagaraṁ āsi,",
  "tha-ap402:22.2_0": "tāvatā jotayissati;",
  "tha-ap402:22.3_0": "Upapajjati yaṁ yoniṁ,",
  "tha-ap402:22.4_0": "devattaṁ atha mānusaṁ.",
  "tha-ap402:23.1_0": "Asseva dīpadānassa,",
  "tha-ap402:23.2_0": "aṭṭhadīpaphalena hi;",
  "tha-ap402:23.3_0": "Na jayissantimaṁ jantū,",
  "tha-ap402:23.4_0": "dīpadānassidaṁ phalaṁ.",
  "tha-ap402:24.1_0": "Kappasatasahassamhi,",
  "tha-ap402:24.2_0": "okkākakulasambhavo;",
  "tha-ap402:24.3_0": "Gotamo nāma gottena,",
  "tha-ap402:24.4_0": "satthā loke bhavissati.",
  "tha-ap402:25.1_0": "Tassa dhammesu dāyādo,",
  "tha-ap402:25.2_0": "oraso dhammanimmito;",
  "tha-ap402:25.3_0": "Sabbāsave pariññāya,",
  "tha-ap402:25.4_0": "nibbāyissatināsavo.",
  "tha-ap402:26.1_0": "Tosayitvāna sambuddhaṁ,",
  "tha-ap402:26.2_0": "gotamaṁ sakyapuṅgavaṁ;",
  "tha-ap402:26.3_0": "Ajito nāma nāmena,",
  "tha-ap402:26.4_0": "hessati satthu sāvako’.",
  "tha-ap402:27.1_0": "Saṭṭhi kappasahassāni,",
  "tha-ap402:27.2_0": "devaloke ramiṁ ahaṁ;",
  "tha-ap402:27.3_0": "Tatrāpi me dīpasataṁ,",
  "tha-ap402:27.4_0": "jotate niccakālikaṁ.",
  "tha-ap402:28.1_0": "Devaloke manusse vā,",
  "tha-ap402:28.2_0": "niddhāvanti pabhā mama;",
  "tha-ap402:28.3_0": "Buddhaseṭṭhaṁ saritvāna,",
  "tha-ap402:28.4_0": "bhiyyo hāsaṁ janesahaṁ.",
  "tha-ap402:29.1_0": "Tusitāhaṁ cavitvāna,",
  "tha-ap402:29.2_0": "okkamiṁ mātukucchiyaṁ;",
  "tha-ap402:29.3_0": "Jāyamānassa santassa,",
  "tha-ap402:29.4_0": "āloko vipulo ahu.",
  "tha-ap402:30.1_0": "Agārā abhinikkhamma,",
  "tha-ap402:30.2_0": "pabbajiṁ anagāriyaṁ;",
  "tha-ap402:30.3_0": "Bāvariṁ upasaṅkamma,",
  "tha-ap402:30.4_0": "sissattaṁ ajjhupāgamiṁ.",
  "tha-ap402:31.1_0": "Himavante vasantohaṁ,",
  "tha-ap402:31.2_0": "assosiṁ lokanāyakaṁ;",
  "tha-ap402:31.3_0": "Uttamatthaṁ gavesanto,",
  "tha-ap402:31.4_0": "upagacchiṁ vināyakaṁ.",
  "tha-ap402:32.1_0": "Danto buddho dametāvī,",
  "tha-ap402:32.2_0": "oghatiṇṇo nirūpadhi;",
  "tha-ap402:32.3_0": "Nibbānaṁ kathayī buddho,",
  "tha-ap402:32.4_0": "sabbadukkhappamocanaṁ.",
  "tha-ap402:33.1_0": "Taṁ me āgamanaṁ siddhaṁ,",
  "tha-ap402:33.2_0": "tositohaṁ mahāmuniṁ;",
  "tha-ap402:33.3_0": "Tisso vijjā anuppattā,",
  "tha-ap402:33.4_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap402:34.1_0": "Satasahassito kappe,",
  "tha-ap402:34.2_0": "yaṁ dīpamadadiṁ tadā;",
  "tha-ap402:34.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap402:34.4_0": "dīpadānassidaṁ phalaṁ.",
  "tha-ap402:35.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap402:35.2_0": "…pe…",
  "tha-ap402:35.3_0": "viharāmi anāsavo.",
  "tha-ap402:36.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap402:36.2_0": "…pe…",
  "tha-ap402:36.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap402:37.1_0": "Paṭisambhidā catasso,",
  "tha-ap402:37.2_0": "…pe…",
  "tha-ap402:37.3_0": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap402:38.1_0": "Itthaṁ sudaṁ āyasmā ajito thero imā gāthāyo abhāsitthāti.",
  "tha-ap402:39.1_0": "Ajitattherassāpadānaṁ dasamaṁ.",
  "tha-ap402:39.2_0": "Pilindavacchavaggo cattālīsamo.",
  "tha-ap402:40.1_0": "Tassuddānaṁ",
  "tha-ap402:41.1_0": "Pilindavaccho selo ca,",
  "tha-ap402:41.2_0": "sabbakittī madhundado;",
  "tha-ap402:41.3_0": "Kūṭāgārī bākulo ca,",
  "tha-ap402:41.4_0": "giri saḷalasavhayo.",
  "tha-ap402:42.1_0": "Sabbado ajito ceva,",
  "tha-ap402:42.2_0": "gāthāyo gaṇitā iha;",
  "tha-ap402:42.3_0": "Satāni pañca gāthānaṁ,",
  "tha-ap402:42.4_0": "vīsati ca taduttarīti.",
  "tha-ap402:43.1_0": "Atha vagguddānaṁ",
  "tha-ap402:44.1_0": "Padumārakkhado ceva,",
  "tha-ap402:44.2_0": "umā gandhodakena ca;",
  "tha-ap402:44.3_0": "Ekapadma saddasaññī,",
  "tha-ap402:44.4_0": "mandāraṁ bodhivandako.",
  "tha-ap402:45.1_0": "Avaṭañca pilindi ca,",
  "tha-ap402:45.2_0": "gāthāyo gaṇitā iha;",
  "tha-ap402:45.3_0": "Catusattati gāthāyo,",
  "tha-ap402:45.4_0": "ekādasa satāni ca.",
  "tha-ap402:46.1_0": "Padumavaggadasakaṁ.",
  "tha-ap402:47.1_0": "Catutthasatakaṁ samattaṁ."
}