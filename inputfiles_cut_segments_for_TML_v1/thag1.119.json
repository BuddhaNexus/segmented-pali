{
  "thag1.119:0.1_0": "Theragāthā",
  "thag1.119:0.2_0": "Ekakanipāta",
  "thag1.119:0.3_0": "Dvādasamavagga",
  "thag1.119:0.4_0": "1.119. Vajjiputtattheragāthā",
  "thag1.119:1.1_0": "“Rukkhamūlagahanaṁ pasakkiya,",
  "thag1.119:1.2_0": "Nibbānaṁ hadayasmiṁ opiya;",
  "thag1.119:1.3_0": "Jhāya gotama mā ca pamādo,",
  "thag1.119:1.4_0": "Kiṁ te biḷibiḷikā karissatī”ti.",
  "thag1.119:1.5_0": "… Vajjiputto thero …."
}