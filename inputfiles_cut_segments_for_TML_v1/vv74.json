{
  "vv74:0.1_0": "Vimānavatthu",
  "vv74:0.2_0": "Purisavimāna",
  "vv74:0.3_0": "Pāyāsivagga",
  "vv74:0.4_0": "10. (Uttara) Pāyāsivimānavatthu",
  "vv74:1.1_0": "“Yā devarājassa sabhā sudhammā,",
  "vv74:1.2_0": "Yatthacchati devasaṅgho samaggo;",
  "vv74:1.3_0": "Tathūpamaṁ tuyhamidaṁ vimānaṁ,",
  "vv74:1.4_0": "Obhāsayaṁ tiṭṭhati antalikkhe.",
  "vv74:2.1_0": "Deviddhipattosi mahānubhāvo,",
  "vv74:2.2_0": "Manussabhūto kimakāsi puññaṁ;",
  "vv74:2.3_0": "Kenāsi evaṁ jalitānubhāvo,",
  "vv74:2.4_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv74:3.1_0": "So devaputto attamano,",
  "vv74:3.2_0": "…pe…",
  "vv74:3.3_0": "yassa kammassidaṁ phalaṁ.",
  "vv74:4.1_0": "“Ahaṁ manussesu manussabhūto,",
  "vv74:4.2_0": "Rañño pāyāsissa ahosiṁ māṇavo;",
  "vv74:4.3_0": "Laddhā dhanaṁ saṁvibhāgaṁ akāsiṁ,",
  "vv74:4.4_0": "Piyā ca me sīlavanto ahesuṁ;",
  "vv74:4.5_0": "Annañca pānañca pasannacitto,",
  "vv74:4.6_0": "Sakkacca dānaṁ vipulaṁ adāsiṁ.",
  "vv74:5.1_0": "Tena metādiso vaṇṇo,",
  "vv74:5.2_0": "…pe…",
  "vv74:5.3_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv74:6.1_0": "Pāyāsivimānaṁ dasamaṁ.",
  "vv74:7.1_0": "Pāyāsivaggo chaṭṭho.",
  "vv74:8.1_0": "Tassuddānaṁ",
  "vv74:9.1_0": "Dve agārino phaladāyī,",
  "vv74:9.2_0": "Dve upassayadāyī bhikkhāya dāyī;",
  "vv74:9.3_0": "Yavapālako ceva dve,",
  "vv74:9.4_0": "Kuṇḍalino pāyāsīti.",
  "vv74:10.1_0": "Purisānaṁ dutiyo vaggo pavuccatīti."
}