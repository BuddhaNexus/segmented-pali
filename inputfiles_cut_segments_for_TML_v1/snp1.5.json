{
  "snp1.5:0.1_0": "Sutta Nipāta 1.5",
  "snp1.5:0.2_0": "Cundasutta",
  "snp1.5:1.1_0": "“Pucchāmi muniṁ pahūtapaññaṁ,",
  "snp1.5:1.2_0": "(iti cundo kammāraputto)",
  "snp1.5:1.3_0": "Buddhaṁ dhammassāmiṁ vītataṇhaṁ;",
  "snp1.5:1.4_0": "Dvipaduttamaṁ sārathīnaṁ pavaraṁ,",
  "snp1.5:1.5_0": "Kati loke samaṇā tadiṅgha brūhi”.",
  "snp1.5:2.1_0": "“Caturo samaṇā na pañcamatthi,",
  "snp1.5:2.2_0": "(cundāti bhagavā)",
  "snp1.5:2.3_0": "Te te āvikaromi sakkhipuṭṭho;",
  "snp1.5:2.4_0": "Maggajino maggadesako ca,",
  "snp1.5:2.5_0": "Magge jīvati yo ca maggadūsī”.",
  "snp1.5:3.1_0": "“Kaṁ maggajinaṁ vadanti buddhā,",
  "snp1.5:3.2_0": "(iti cundo kammāraputto)",
  "snp1.5:3.3_0": "Maggakkhāyī kathaṁ atulyo hoti;",
  "snp1.5:3.4_0": "Magge jīvati me brūhi puṭṭho,",
  "snp1.5:3.5_0": "Atha me āvikarohi maggadūsiṁ”.",
  "snp1.5:4.1_0": "“Yo tiṇṇakathaṅkatho visallo,",
  "snp1.5:4.2_0": "Nibbānābhirato anānugiddho;",
  "snp1.5:4.3_0": "Lokassa sadevakassa netā,",
  "snp1.5:4.4_0": "Tādiṁ maggajinaṁ vadanti buddhā.",
  "snp1.5:5.1_0": "Paramaṁ paramanti yodha ñatvā,",
  "snp1.5:5.2_0": "Akkhāti vibhajate idheva dhammaṁ;",
  "snp1.5:5.3_0": "Taṁ kaṅkhachidaṁ muniṁ anejaṁ,",
  "snp1.5:5.4_0": "Dutiyaṁ bhikkhunamāhu maggadesiṁ.",
  "snp1.5:6.1_0": "Yo dhammapade sudesite,",
  "snp1.5:6.2_0": "Magge jīvati saññato satīmā;",
  "snp1.5:6.3_0": "Anavajjapadāni sevamāno,",
  "snp1.5:6.4_0": "Tatiyaṁ bhikkhunamāhu maggajīviṁ.",
  "snp1.5:7.1_0": "Chadanaṁ katvāna subbatānaṁ,",
  "snp1.5:7.2_0": "Pakkhandī kuladūsako pagabbho;",
  "snp1.5:7.3_0": "Māyāvī asaññato palāpo,",
  "snp1.5:7.4_0": "Patirūpena caraṁ sa maggadūsī.",
  "snp1.5:8.1_0": "Ete ca paṭivijjhi yo gahaṭṭho,",
  "snp1.5:8.2_0": "Sutavā ariyasāvako sapañño;",
  "snp1.5:8.3_0": "Sabbe netādisāti ñatvā,",
  "snp1.5:8.4_0": "Iti disvā na hāpeti tassa saddhā;",
  "snp1.5:8.5_0": "Kathaṁ hi duṭṭhena asampaduṭṭhaṁ,",
  "snp1.5:8.6_0": "Suddhaṁ asuddhena samaṁ kareyyā”ti.",
  "snp1.5:8.7_0": "Cundasuttaṁ pañcamaṁ."
}