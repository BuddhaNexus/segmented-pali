{
  "thag5.10:0.1_0": "Theragāthā",
  "thag5.10:0.2_0": "Pañcakanipāta",
  "thag5.10:0.3_0": "Paṭhamavagga",
  "thag5.10:0.4_0": "5.10. Yasadattattheragāthā",
  "thag5.10:1.1_0": "“Upārambhacitto dummedho,",
  "thag5.10:1.2_0": "suṇāti jinasāsanaṁ;",
  "thag5.10:1.3_0": "Ārakā hoti saddhammā,",
  "thag5.10:1.4_0": "nabhaso pathavī yathā.",
  "thag5.10:2.1_0": "Upārambhacitto dummedho,",
  "thag5.10:2.2_0": "suṇāti jinasāsanaṁ;",
  "thag5.10:2.3_0": "Parihāyati saddhammā,",
  "thag5.10:2.4_0": "kāḷapakkheva candimā.",
  "thag5.10:3.1_0": "Upārambhacitto dummedho,",
  "thag5.10:3.2_0": "suṇāti jinasāsanaṁ;",
  "thag5.10:3.3_0": "Parisussati saddhamme,",
  "thag5.10:3.4_0": "maccho appodake yathā.",
  "thag5.10:4.1_0": "Upārambhacitto dummedho,",
  "thag5.10:4.2_0": "suṇāti jinasāsanaṁ;",
  "thag5.10:4.3_0": "Na virūhati saddhamme,",
  "thag5.10:4.4_0": "khette bījaṁva pūtikaṁ.",
  "thag5.10:5.1_0": "Yo ca tuṭṭhena cittena,",
  "thag5.10:5.2_0": "suṇāti jinasāsanaṁ;",
  "thag5.10:5.3_0": "Khepetvā āsave sabbe,",
  "thag5.10:5.4_0": "sacchikatvā akuppataṁ;",
  "thag5.10:5.5_0": "Pappuyya paramaṁ santiṁ,",
  "thag5.10:5.6_0": "parinibbātināsavo”ti.",
  "thag5.10:5.7_0": "… Yasadatto thero …."
}