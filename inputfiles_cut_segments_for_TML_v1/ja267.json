{
  "ja267:0.1_0": "Jātaka",
  "ja267:0.2_0": "Tikanipāta",
  "ja267:0.3_0": "Padumavagga",
  "ja267:0.4_0": "7. Kakkaṭakajātaka",
  "ja267:1.1_0": "“Siṅgīmigo āyatacakkhunetto,",
  "ja267:1.2_0": "Aṭṭhittaco vārisayo alomo;",
  "ja267:1.3_0": "Tenābhibhūto kapaṇaṁ rudāmi,",
  "ja267:1.4_0": "Mā heva maṁ pāṇasamaṁ jaheyya”.",
  "ja267:2.1_0": "“Ayya na taṁ jahissāmi,",
  "ja267:2.2_0": "kuñjaraṁ saṭṭhihāyanaṁ;",
  "ja267:2.3_0": "Pathabyā cāturantāya,",
  "ja267:2.4_0": "suppiyo hosi me tuvaṁ”.",
  "ja267:3.1_0": "“Ye kuḷīrā samuddasmiṁ,",
  "ja267:3.2_0": "gaṅgāya yamunāya ca;",
  "ja267:3.3_0": "Tesaṁ tvaṁ vārijo seṭṭho,",
  "ja267:3.4_0": "muñca rodantiyā patin”ti.",
  "ja267:4.1_0": "Kakkaṭakajātakaṁ sattamaṁ."
}