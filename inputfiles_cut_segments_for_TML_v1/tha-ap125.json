{
  "tha-ap125:0.1_0": "Therāpadāna",
  "tha-ap125:0.2_0": "Sereyyavagga",
  "tha-ap125:0.3_0": "3 Pāyasadāyakattheraapadāna",
  "tha-ap125:1.1_0": "“Suvaṇṇavaṇṇo sambuddho,",
  "tha-ap125:1.2_0": "Bāttiṁsavaralakkhaṇo;",
  "tha-ap125:1.3_0": "Pavanā abhinikkhanto,",
  "tha-ap125:1.4_0": "Bhikkhusaṅghapurakkhato.",
  "tha-ap125:2.1_0": "Mahaccā kaṁsapātiyā,",
  "tha-ap125:2.2_0": "vaḍḍhetvā pāyasaṁ ahaṁ;",
  "tha-ap125:2.3_0": "Āhutiṁ yiṭṭhukāmo so,",
  "tha-ap125:2.4_0": "upanesiṁ baliṁ ahaṁ.",
  "tha-ap125:3.1_0": "Bhagavā tamhi samaye,",
  "tha-ap125:3.2_0": "lokajeṭṭho narāsabho;",
  "tha-ap125:3.3_0": "Caṅkamaṁ susamārūḷho,",
  "tha-ap125:3.4_0": "ambare anilāyane.",
  "tha-ap125:4.1_0": "Tañca acchariyaṁ disvā,",
  "tha-ap125:4.2_0": "abbhutaṁ lomahaṁsanaṁ;",
  "tha-ap125:4.3_0": "Ṭhapayitvā kaṁsapātiṁ,",
  "tha-ap125:4.4_0": "vipassiṁ abhivādayiṁ.",
  "tha-ap125:5.1_0": "Tuvaṁ devosi sabbaññū,",
  "tha-ap125:5.2_0": "sadeve sahamānuse;",
  "tha-ap125:5.3_0": "Anukampaṁ upādāya,",
  "tha-ap125:5.4_0": "paṭigaṇha mahāmuni.",
  "tha-ap125:6.1_0": "Paṭiggahesi bhagavā,",
  "tha-ap125:6.2_0": "sabbaññū lokanāyako;",
  "tha-ap125:6.3_0": "Mama saṅkappamaññāya,",
  "tha-ap125:6.4_0": "satthā loke mahāmuni.",
  "tha-ap125:7.1_0": "Ekanavutito kappe,",
  "tha-ap125:7.2_0": "yaṁ dānamadadiṁ tadā;",
  "tha-ap125:7.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap125:7.4_0": "pāyasassa idaṁ phalaṁ.",
  "tha-ap125:8.1_0": "Ekatālīsito kappe,",
  "tha-ap125:8.2_0": "buddho nāmāsi khattiyo;",
  "tha-ap125:8.3_0": "Sattaratanasampanno,",
  "tha-ap125:8.4_0": "cakkavattī mahabbalo.",
  "tha-ap125:9.1_0": "Paṭisambhidā catasso,",
  "tha-ap125:9.2_0": "…pe…",
  "tha-ap125:9.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap125:10.1_0": "Itthaṁ sudaṁ āyasmā pāyasadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap125:11.1_0": "Pāyasadāyakattherassāpadānaṁ tatiyaṁ."
}