{
  "thag2.13:0.1_0": "Theragāthā",
  "thag2.13:0.2_0": "Dukanipāta",
  "thag2.13:0.3_0": "Dutiyavagga",
  "thag2.13:0.4_0": "2.13. Heraññakānittheragāthā",
  "thag2.13:1.1_0": "“Accayanti ahorattā,",
  "thag2.13:1.2_0": "jīvitaṁ uparujjhati;",
  "thag2.13:1.3_0": "Āyu khīyati maccānaṁ,",
  "thag2.13:1.4_0": "kunnadīnaṁva odakaṁ.",
  "thag2.13:2.1_0": "Atha pāpāni kammāni,",
  "thag2.13:2.2_0": "karaṁ bālo na bujjhati;",
  "thag2.13:2.3_0": "Pacchāssa kaṭukaṁ hoti,",
  "thag2.13:2.4_0": "vipāko hissa pāpako”ti.",
  "thag2.13:2.5_0": "… Heraññakānitthero …."
}