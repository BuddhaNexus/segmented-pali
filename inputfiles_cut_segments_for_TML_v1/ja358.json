{
  "ja358:0.1_0": "Jātaka",
  "ja358:0.2_0": "Pañcakanipāta",
  "ja358:0.3_0": "Maṇikuṇḍalavagga",
  "ja358:0.4_0": "8. Cūḷadhammapālajātaka",
  "ja358:1.1_0": "“Ahameva dūsiyā bhūnahatā,",
  "ja358:1.2_0": "Rañño mahāpatāpassa;",
  "ja358:1.3_0": "Etaṁ muñcatu dhammapālaṁ,",
  "ja358:1.4_0": "Hatthe me deva chedehi.",
  "ja358:2.1_0": "Ahameva dūsiyā bhūnahatā,",
  "ja358:2.2_0": "Rañño mahāpatāpassa;",
  "ja358:2.3_0": "Etaṁ muñcatu dhammapālaṁ,",
  "ja358:2.4_0": "Pāde me deva chedehi.",
  "ja358:3.1_0": "Ahameva dūsiyā bhūnahatā,",
  "ja358:3.2_0": "Rañño mahāpatāpassa;",
  "ja358:3.3_0": "Etaṁ muñcatu dhammapālaṁ,",
  "ja358:3.4_0": "Sīsaṁ me deva chedehi.",
  "ja358:4.1_0": "Na hi nūnimassa rañño,",
  "ja358:4.2_0": "Mittāmaccā ca vijjare suhadā;",
  "ja358:4.3_0": "Ye na vadanti rājānaṁ,",
  "ja358:4.4_0": "Mā ghātayi orasaṁ puttaṁ.",
  "ja358:5.1_0": "Na hi nūnimassa rañño,",
  "ja358:5.2_0": "Ñātī mittā ca vijjare suhadā;",
  "ja358:5.3_0": "Ye na vadanti rājānaṁ,",
  "ja358:5.4_0": "Mā ghātayi atrajaṁ puttaṁ.",
  "ja358:6.1_0": "Candanasārānulittā,",
  "ja358:6.2_0": "Bāhā chijjanti dhammapālassa;",
  "ja358:6.3_0": "Dāyādassa pathabyā,",
  "ja358:6.4_0": "Pāṇā me deva rujjhantī”ti.",
  "ja358:7.1_0": "Cūḷadhammapālajātakaṁ aṭṭhamaṁ."
}