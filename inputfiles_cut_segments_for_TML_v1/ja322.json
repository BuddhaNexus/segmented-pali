{
  "ja322:0.1_0": "Jātaka",
  "ja322:0.2_0": "Catukkanipāta",
  "ja322:0.3_0": "Kuṭidūsakavagga",
  "ja322:0.4_0": "2. Duddubhajātaka",
  "ja322:1.1_0": "“Duddubhāyati bhaddante,",
  "ja322:1.2_0": "yasmiṁ dese vasāmahaṁ;",
  "ja322:1.3_0": "Ahampetaṁ na jānāmi,",
  "ja322:1.4_0": "kimetaṁ duddubhāyati”.",
  "ja322:2.1_0": "“Beluvaṁ patitaṁ sutvā,",
  "ja322:2.2_0": "duddubhanti saso javi;",
  "ja322:2.3_0": "Sasassa vacanaṁ sutvā,",
  "ja322:2.4_0": "santattā migavāhinī.",
  "ja322:3.1_0": "Appatvā padaviññāṇaṁ,",
  "ja322:3.2_0": "paraghosānusārino;",
  "ja322:3.3_0": "Panādaparamā bālā,",
  "ja322:3.4_0": "te honti parapattiyā.",
  "ja322:4.1_0": "Ye ca sīlena sampannā,",
  "ja322:4.2_0": "paññāyūpasame ratā;",
  "ja322:4.3_0": "Ārakā viratā dhīrā,",
  "ja322:4.4_0": "na honti parapattiyā”ti.",
  "ja322:5.1_0": "Duddubhajātakaṁ dutiyaṁ."
}