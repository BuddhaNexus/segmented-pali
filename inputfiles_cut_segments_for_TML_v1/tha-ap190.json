{
  "tha-ap190:0.1_0": "Therāpadāna",
  "tha-ap190:0.2_0": "Kuṭajapupphiyavagga",
  "tha-ap190:0.3_0": "8. Tikaṇṇipupphiyattheraapadāna",
  "tha-ap190:1.1_0": "“Devabhūto ahaṁ santo,",
  "tha-ap190:1.2_0": "accharāhi purakkhato;",
  "tha-ap190:1.3_0": "Pubbakammaṁ saritvāna,",
  "tha-ap190:1.4_0": "buddhaseṭṭhaṁ anussariṁ.",
  "tha-ap190:2.1_0": "Tikaṇṇipupphaṁ paggayha,",
  "tha-ap190:2.2_0": "sakaṁ cittaṁ pasādayiṁ;",
  "tha-ap190:2.3_0": "Buddhamhi abhiropesiṁ,",
  "tha-ap190:2.4_0": "vipassimhi narāsabhe.",
  "tha-ap190:3.1_0": "Ekanavutito kappe,",
  "tha-ap190:3.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap190:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap190:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap190:4.1_0": "Tesattatimhito kappe,",
  "tha-ap190:4.2_0": "caturāsuṁ ramuttamā;",
  "tha-ap190:4.3_0": "Sattaratanasampannā,",
  "tha-ap190:4.4_0": "cakkavattī mahabbalā.",
  "tha-ap190:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap190:5.2_0": "…pe…",
  "tha-ap190:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap190:6.1_0": "Itthaṁ sudaṁ āyasmā tikaṇṇipupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap190:7.1_0": "Tikaṇṇipupphiyattherassāpadānaṁ aṭṭhamaṁ."
}