{
  "kv6.1:0.1_0": "Kathāvatthu",
  "kv6.1:0.2_0": "Niyāmapaṇṇāsaka",
  "kv6.1:0.3_0": "Chaṭṭhavagga",
  "kv6.1:0.4_0": "Niyāmakathā",
  "kv6.1:1.1_0": "Niyāmo asaṅkhatoti?",
  "kv6.1:1.2_0": "Āmantā.",
  "kv6.1:1.3_0": "Nibbānaṁ tāṇaṁ leṇaṁ saraṇaṁ parāyanaṁ accutaṁ amatanti?",
  "kv6.1:1.4_0": "Na hevaṁ vattabbe …pe….",
  "kv6.1:2.1_0": "Niyāmo asaṅkhato, nibbānaṁ asaṅkhatanti?",
  "kv6.1:2.2_0": "Āmantā.",
  "kv6.1:2.3_0": "Dve asaṅkhatānīti?",
  "kv6.1:2.4_0": "Na hevaṁ vattabbe …pe….",
  "kv6.1:3.1_0": "Dve asaṅkhatānīti?",
  "kv6.1:3.2_0": "Āmantā.",
  "kv6.1:3.3_0": "Dve tāṇāni dve leṇāni dve saraṇāni dve parāyanāni dve accutāni dve amatāni dve nibbānānīti?",
  "kv6.1:3.4_0": "Na hevaṁ vattabbe …pe….",
  "kv6.1:4.1_0": "Dve nibbānānīti?",
  "kv6.1:4.2_0": "Āmantā.",
  "kv6.1:4.3_0": "Atthi dvinnaṁ nibbānānaṁ uccanīcatā hīnapaṇītatā ukkaṁsāvakaṁso sīmā vā bhedo vā rāji vā antarikā vāti?",
  "kv6.1:4.4_0": "Na hevaṁ vattabbe …pe….",
  "kv6.1:5.1_0": "Niyāmo asaṅkhatoti?",
  "kv6.1:5.2_0": "Āmantā.",
  "kv6.1:5.3_0": "Atthi keci niyāmaṁ okkamanti paṭilabhanti uppādenti samuppādenti uṭṭhāpenti samuṭṭhāpenti nibbattenti abhinibbattenti janenti sañjanentīti?",
  "kv6.1:5.4_0": "Āmantā.",
  "kv6.1:5.5_0": "Atthi keci asaṅkhataṁ okkamanti paṭilabhanti uppādenti samuppādenti uṭṭhāpenti samuṭṭhāpenti nibbattenti abhinibbattenti janenti sañjanentīti?",
  "kv6.1:5.6_0": "Na hevaṁ vattabbe …pe….",
  "kv6.1:6.1_0": "Niyāmo asaṅkhatoti?",
  "kv6.1:6.2_0": "Āmantā.",
  "kv6.1:6.3_0": "Maggo asaṅkhatoti?",
  "kv6.1:6.4_0": "Na hevaṁ vattabbe …pe….",
  "kv6.1:7.1_0": "Maggo saṅkhatoti?",
  "kv6.1:7.2_0": "Āmantā.",
  "kv6.1:7.3_0": "Niyāmo saṅkhatoti?",
  "kv6.1:7.4_0": "Na hevaṁ vattabbe …pe….",
  "kv6.1:8.1_0": "Sotāpattiniyāmo asaṅkhatoti?",
  "kv6.1:8.2_0": "Āmantā.",
  "kv6.1:8.3_0": "Sotāpattimaggo asaṅkhatoti?",
  "kv6.1:8.4_0": "Na hevaṁ vattabbe …pe….",
  "kv6.1:9.1_0": "Sotāpattimaggo saṅkhatoti?",
  "kv6.1:9.2_0": "Āmantā.",
  "kv6.1:9.3_0": "Sotāpattiniyāmo saṅkhatoti?",
  "kv6.1:9.4_0": "Na hevaṁ vattabbe …pe….",
  "kv6.1:10.1_0": "Sakadāgāminiyāmo …pe…",
  "kv6.1:10.2_0": "anāgāminiyāmo …pe…",
  "kv6.1:10.3_0": "arahattaniyāmo asaṅkhatoti?",
  "kv6.1:10.4_0": "Āmantā.",
  "kv6.1:10.5_0": "Arahattamaggo asaṅkhatoti?",
  "kv6.1:10.6_0": "Na hevaṁ vattabbe …pe…",
  "kv6.1:10.7_0": "arahattamaggo saṅkhatoti?",
  "kv6.1:10.8_0": "Āmantā.",
  "kv6.1:10.9_0": "Arahattaniyāmo saṅkhatoti?",
  "kv6.1:10.10_0": "Na hevaṁ vattabbe …pe….",
  "kv6.1:11.1_0": "Sotāpattiniyāmo asaṅkhato …pe…",
  "kv6.1:11.2_0": "arahattaniyāmo asaṅkhato, nibbānaṁ asaṅkhatanti?",
  "kv6.1:11.3_0": "Āmantā.",
  "kv6.1:11.4_0": "Pañca asaṅkhatānīti?",
  "kv6.1:11.5_0": "Na hevaṁ vattabbe …pe…",
  "kv6.1:11.6_0": "pañca asaṅkhatānīti?",
  "kv6.1:11.7_0": "Āmantā.",
  "kv6.1:11.8_0": "Pañca tāṇāni …pe…",
  "kv6.1:11.9_0": "antarikā vāti?",
  "kv6.1:11.10_0": "Na hevaṁ vattabbe …pe….",
  "kv6.1:12.1_0": "Niyāmo asaṅkhatoti?",
  "kv6.1:12.2_0": "Āmantā.",
  "kv6.1:12.3_0": "Micchattaniyāmo asaṅkhatoti?",
  "kv6.1:12.4_0": "Na hevaṁ vattabbe …pe…",
  "kv6.1:12.5_0": "micchattaniyāmo saṅkhatoti?",
  "kv6.1:12.6_0": "Āmantā.",
  "kv6.1:12.7_0": "Sammattaniyāmo saṅkhatoti?",
  "kv6.1:12.8_0": "Na hevaṁ vattabbe …pe….",
  "kv6.1:13.1_0": "Na vattabbaṁ—",
  "kv6.1:13.2_0": "“niyāmo asaṅkhato”ti?",
  "kv6.1:13.3_0": "Āmantā.",
  "kv6.1:13.4_0": "Niyāme uppajja niruddhe aniyato hotīti?",
  "kv6.1:13.5_0": "Na hevaṁ vattabbe …pe….",
  "kv6.1:13.6_0": "Tena hi niyāmo asaṅkhatoti.",
  "kv6.1:13.7_0": "Micchattaniyāme uppajja niruddhe aniyato hotīti?",
  "kv6.1:13.8_0": "Na hevaṁ vattabbe …pe…",
  "kv6.1:13.9_0": "tena hi micchattaniyāmo asaṅkhatoti.",
  "kv6.1:13.10_0": "Niyāmakathā niṭṭhitā."
}