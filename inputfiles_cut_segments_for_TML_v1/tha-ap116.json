{
  "tha-ap116:0.1_0": "Therāpadāna",
  "tha-ap116:0.2_0": "Mahāparivāravagga",
  "tha-ap116:0.3_0": "4. Ekāsaniyattheraapadāna",
  "tha-ap116:1.1_0": "“Varuṇo nāma nāmena,",
  "tha-ap116:1.2_0": "devarājā ahaṁ tadā;",
  "tha-ap116:1.3_0": "Upaṭṭhahesiṁ sambuddhaṁ,",
  "tha-ap116:1.4_0": "sayoggabalavāhano.",
  "tha-ap116:2.1_0": "Nibbute lokanāthamhi,",
  "tha-ap116:2.2_0": "atthadassīnaruttame;",
  "tha-ap116:2.3_0": "Tūriyaṁ sabbamādāya,",
  "tha-ap116:2.4_0": "agamaṁ bodhimuttamaṁ.",
  "tha-ap116:3.1_0": "Vāditena ca naccena,",
  "tha-ap116:3.2_0": "sammatāḷasamāhito;",
  "tha-ap116:3.3_0": "Sammukhā viya sambuddhaṁ,",
  "tha-ap116:3.4_0": "upaṭṭhiṁ bodhimuttamaṁ.",
  "tha-ap116:4.1_0": "Upaṭṭhahitvā taṁ bodhiṁ,",
  "tha-ap116:4.2_0": "dharaṇīruhapādapaṁ;",
  "tha-ap116:4.3_0": "Pallaṅkaṁ ābhujitvāna,",
  "tha-ap116:4.4_0": "tattha kālaṅkato ahaṁ.",
  "tha-ap116:5.1_0": "Sakakammābhiraddhohaṁ,",
  "tha-ap116:5.2_0": "pasanno bodhimuttame;",
  "tha-ap116:5.3_0": "Tena cittappasādena,",
  "tha-ap116:5.4_0": "nimmānaṁ upapajjahaṁ.",
  "tha-ap116:6.1_0": "Saṭṭhitūriyasahassāni,",
  "tha-ap116:6.2_0": "parivārenti maṁ sadā;",
  "tha-ap116:6.3_0": "Manussesu ca devesu,",
  "tha-ap116:6.4_0": "vattamānaṁ bhavābhave.",
  "tha-ap116:7.1_0": "Tividhaggī nibbutā mayhaṁ,",
  "tha-ap116:7.2_0": "bhavā sabbe samūhatā;",
  "tha-ap116:7.3_0": "Dhāremi antimaṁ dehaṁ,",
  "tha-ap116:7.4_0": "sammāsambuddhasāsane.",
  "tha-ap116:8.1_0": "Subāhū nāma nāmena,",
  "tha-ap116:8.2_0": "catuttiṁsāsu khattiyā;",
  "tha-ap116:8.3_0": "Sattaratanasampannā,",
  "tha-ap116:8.4_0": "pañcakappasate ito.",
  "tha-ap116:9.1_0": "Paṭisambhidā catasso,",
  "tha-ap116:9.2_0": "…pe…",
  "tha-ap116:9.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap116:10.1_0": "Itthaṁ sudaṁ āyasmā ekāsaniyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap116:11.1_0": "Ekāsaniyattherassāpadānaṁ catutthaṁ."
}