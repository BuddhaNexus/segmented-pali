{
  "tha-ap298:0.1_0": "Therāpadāna",
  "tha-ap298:0.2_0": "Citakapūjakavagga",
  "tha-ap298:0.3_0": "6 Pādapūjakattheraapadāna",
  "tha-ap298:1.1_0": "“Pabbate himavantamhi,",
  "tha-ap298:1.2_0": "ahosiṁ kinnaro tadā;",
  "tha-ap298:1.3_0": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap298:1.4_0": "pītaraṁsiṁva bhāṇumaṁ.",
  "tha-ap298:2.1_0": "Upetaṁ tamahaṁ buddhaṁ,",
  "tha-ap298:2.2_0": "vipassiṁ lokanāyakaṁ;",
  "tha-ap298:2.3_0": "Candanaṁ tagarañcāpi,",
  "tha-ap298:2.4_0": "pāde osiñcahaṁ tadā.",
  "tha-ap298:3.1_0": "Ekanavutito kappe,",
  "tha-ap298:3.2_0": "yaṁ pādaṁ abhipūjayiṁ;",
  "tha-ap298:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap298:3.4_0": "pādapūjāyidaṁ phalaṁ.",
  "tha-ap298:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap298:4.2_0": "…pe…",
  "tha-ap298:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap298:5.1_0": "Itthaṁ sudaṁ āyasmā pādapūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap298:6.1_0": "Pādapūjakattherassāpadānaṁ chaṭṭhaṁ."
}