{
  "ja428:0.1_0": "Jātaka",
  "ja428:0.2_0": "Navakanipāta",
  "ja428:0.3_0": "Gijjhavagga",
  "ja428:0.4_0": "2. Kosambiyajātaka",
  "ja428:1.1_0": "“Puthusaddo samajano,",
  "ja428:1.2_0": "na bālo koci maññatha;",
  "ja428:1.3_0": "Saṅghasmiṁ bhijjamānasmiṁ,",
  "ja428:1.4_0": "nāññaṁ bhiyyo amaññaruṁ.",
  "ja428:2.1_0": "Parimuṭṭhā paṇḍitābhāsā,",
  "ja428:2.2_0": "vācāgocarabhāṇino;",
  "ja428:2.3_0": "Yāvicchanti mukhāyāmaṁ,",
  "ja428:2.4_0": "yena nītā na taṁ vidū.",
  "ja428:3.1_0": "Akkocchi maṁ avadhi maṁ,",
  "ja428:3.2_0": "ajini maṁ ahāsi me;",
  "ja428:3.3_0": "Ye ca taṁ upanayhanti,",
  "ja428:3.4_0": "veraṁ tesaṁ na sammati.",
  "ja428:4.1_0": "Akkocchi maṁ avadhi maṁ,",
  "ja428:4.2_0": "ajini maṁ ahāsi me;",
  "ja428:4.3_0": "Ye ca taṁ nupanayhanti,",
  "ja428:4.4_0": "veraṁ tesūpasammati.",
  "ja428:5.1_0": "Na hi verena verāni,",
  "ja428:5.2_0": "sammantīdha kudācanaṁ;",
  "ja428:5.3_0": "Averena ca sammanti,",
  "ja428:5.4_0": "esa dhammo sanantano.",
  "ja428:6.1_0": "Pare ca na vijānanti,",
  "ja428:6.2_0": "mayamettha yamāmase;",
  "ja428:6.3_0": "Ye ca tattha vijānanti,",
  "ja428:6.4_0": "tato sammanti medhagā.",
  "ja428:7.1_0": "Aṭṭhicchinnā pāṇaharā,",
  "ja428:7.2_0": "gavāssa dhanahārino;",
  "ja428:7.3_0": "Raṭṭhaṁ vilumpamānānaṁ,",
  "ja428:7.4_0": "tesampi hoti saṅgati;",
  "ja428:7.5_0": "Kasmā tumhāka no siyā.",
  "ja428:8.1_0": "Sace labhetha nipakaṁ sahāyaṁ,",
  "ja428:8.2_0": "Saddhiṁ caraṁ sādhuvihāridhīraṁ;",
  "ja428:8.3_0": "Abhibhuyya sabbāni parissayāni,",
  "ja428:8.4_0": "Careyya tenattamano satīmā.",
  "ja428:9.1_0": "No ce labhetha nipakaṁ sahāyaṁ,",
  "ja428:9.2_0": "Saddhiṁ caraṁ sādhuvihāridhīraṁ;",
  "ja428:9.3_0": "Rājāva raṭṭhaṁ vijitaṁ pahāya,",
  "ja428:9.4_0": "Eko care mātaṅgaraññeva nāgo.",
  "ja428:10.1_0": "Ekassa caritaṁ seyyo,",
  "ja428:10.2_0": "Natthi bāle sahāyatā;",
  "ja428:10.3_0": "Eko care na pāpāni kayirā,",
  "ja428:10.4_0": "Appossukko mātaṅgaraññeva nāgo”ti.",
  "ja428:11.1_0": "Kosambiyajātakaṁ dutiyaṁ."
}