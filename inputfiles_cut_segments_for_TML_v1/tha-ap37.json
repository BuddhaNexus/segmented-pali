{
  "tha-ap37:0.1_0": "Therāpadāna",
  "tha-ap37:0.2_0": "Kuṇḍadhānavagga",
  "tha-ap37:0.3_0": "5. Mogharājattheraapadāna",
  "tha-ap37:1.1_0": "“Atthadassī tu bhagavā,",
  "tha-ap37:1.2_0": "sayambhū aparājito;",
  "tha-ap37:1.3_0": "Bhikkhusaṅghaparibyūḷho,",
  "tha-ap37:1.4_0": "rathiyaṁ paṭipajjatha.",
  "tha-ap37:2.1_0": "Sissehi samparivuto,",
  "tha-ap37:2.2_0": "gharamhā abhinikkhamiṁ;",
  "tha-ap37:2.3_0": "Nikkhamitvānahaṁ tattha,",
  "tha-ap37:2.4_0": "addasaṁ lokanāyakaṁ.",
  "tha-ap37:3.1_0": "Abhivādiya sambuddhaṁ,",
  "tha-ap37:3.2_0": "sire katvāna añjaliṁ;",
  "tha-ap37:3.3_0": "Sakaṁ cittaṁ pasādetvā,",
  "tha-ap37:3.4_0": "santhaviṁ lokanāyakaṁ.",
  "tha-ap37:4.1_0": "Yāvatā rūpino sattā,",
  "tha-ap37:4.2_0": "arūpī vā asaññino;",
  "tha-ap37:4.3_0": "Sabbe te tava ñāṇamhi,",
  "tha-ap37:4.4_0": "anto honti samogadhā.",
  "tha-ap37:5.1_0": "Sukhumacchikajālena,",
  "tha-ap37:5.2_0": "udakaṁ yo parikkhipe;",
  "tha-ap37:5.3_0": "Ye keci udake pāṇā,",
  "tha-ap37:5.4_0": "antojāle bhavanti te.",
  "tha-ap37:6.1_0": "Yesañca cetanā atthi,",
  "tha-ap37:6.2_0": "rūpino ca arūpino;",
  "tha-ap37:6.3_0": "Sabbe te tava ñāṇamhi,",
  "tha-ap37:6.4_0": "anto honti samogadhā.",
  "tha-ap37:7.1_0": "Samuddharasimaṁ lokaṁ,",
  "tha-ap37:7.2_0": "andhakārasamākulaṁ;",
  "tha-ap37:7.3_0": "Tava dhammaṁ suṇitvāna,",
  "tha-ap37:7.4_0": "kaṅkhāsotaṁ taranti te.",
  "tha-ap37:8.1_0": "Avijjānivute loke,",
  "tha-ap37:8.2_0": "andhakārena otthaṭe;",
  "tha-ap37:8.3_0": "Tava ñāṇamhi jotante,",
  "tha-ap37:8.4_0": "andhakārā padhaṁsitā.",
  "tha-ap37:9.1_0": "Tuvaṁ cakkhūsi sabbesaṁ,",
  "tha-ap37:9.2_0": "mahātamapanūdano;",
  "tha-ap37:9.3_0": "Tava dhammaṁ suṇitvāna,",
  "tha-ap37:9.4_0": "nibbāyati bahujjano.",
  "tha-ap37:10.1_0": "Puṭakaṁ pūrayitvāna,",
  "tha-ap37:10.2_0": "madhukhuddamaneḷakaṁ;",
  "tha-ap37:10.3_0": "Ubho hatthehi paggayha,",
  "tha-ap37:10.4_0": "upanesiṁ mahesino.",
  "tha-ap37:11.1_0": "Paṭiggaṇhi mahāvīro,",
  "tha-ap37:11.2_0": "sahatthena mahā isi;",
  "tha-ap37:11.3_0": "Bhuñjitvā tañca sabbaññū,",
  "tha-ap37:11.4_0": "vehāsaṁ nabhamuggami.",
  "tha-ap37:12.1_0": "Antalikkhe ṭhito satthā,",
  "tha-ap37:12.2_0": "atthadassī narāsabho;",
  "tha-ap37:12.3_0": "Mama cittaṁ pasādento,",
  "tha-ap37:12.4_0": "imā gāthā abhāsatha.",
  "tha-ap37:13.1_0": "‘Yenidaṁ thavitaṁ ñāṇaṁ,",
  "tha-ap37:13.2_0": "buddhaseṭṭho ca thomito;",
  "tha-ap37:13.3_0": "Tena cittappasādena,",
  "tha-ap37:13.4_0": "duggatiṁ so na gacchati.",
  "tha-ap37:14.1_0": "Catuddasañcakkhattuṁ so,",
  "tha-ap37:14.2_0": "devarajjaṁ karissati;",
  "tha-ap37:14.3_0": "Pathabyā rajjaṁ aṭṭhasataṁ,",
  "tha-ap37:14.4_0": "vasudhaṁ āvasissati.",
  "tha-ap37:15.1_0": "Pañceva satakkhattuñca,",
  "tha-ap37:15.2_0": "cakkavattī bhavissati;",
  "tha-ap37:15.3_0": "Padesarajjaṁ asaṅkheyyaṁ,",
  "tha-ap37:15.4_0": "mahiyā kārayissati.",
  "tha-ap37:16.1_0": "Ajjhāyako mantadharo,",
  "tha-ap37:16.2_0": "tiṇṇaṁ vedāna pāragū;",
  "tha-ap37:16.3_0": "Gotamassa bhagavato,",
  "tha-ap37:16.4_0": "sāsane pabbajissati.",
  "tha-ap37:17.1_0": "Gambhīraṁ nipuṇaṁ atthaṁ,",
  "tha-ap37:17.2_0": "ñāṇena vicinissati;",
  "tha-ap37:17.3_0": "Mogharājāti nāmena,",
  "tha-ap37:17.4_0": "hessati satthu sāvako.",
  "tha-ap37:18.1_0": "Tīhi vijjāhi sampannaṁ,",
  "tha-ap37:18.2_0": "katakiccamanāsavaṁ;",
  "tha-ap37:18.3_0": "Gotamo satthavāhaggo,",
  "tha-ap37:18.4_0": "etadagge ṭhapessati’.",
  "tha-ap37:19.1_0": "Hitvā mānusakaṁ yogaṁ,",
  "tha-ap37:19.2_0": "chetvāna bhavabandhanaṁ;",
  "tha-ap37:19.3_0": "Sabbāsave pariññāya,",
  "tha-ap37:19.4_0": "viharāmi anāsavo.",
  "tha-ap37:20.1_0": "Paṭisambhidā catasso,",
  "tha-ap37:20.2_0": "…pe…",
  "tha-ap37:20.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap37:21.1_0": "Itthaṁ sudaṁ āyasmā mogharājo thero imā gāthāyo abhāsitthāti.",
  "tha-ap37:22.1_0": "Mogharājattherassāpadānaṁ pañcamaṁ."
}