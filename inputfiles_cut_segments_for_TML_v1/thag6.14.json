{
  "thag6.14:0.1_0": "Theragāthā",
  "thag6.14:0.2_0": "Chakkanipāta",
  "thag6.14:0.3_0": "Paṭhamavagga",
  "thag6.14:0.4_0": "6.14. Sabbakāmittheragāthā",
  "thag6.14:1.1_0": "“Dvipādakoyaṁ asuci,",
  "thag6.14:1.2_0": "duggandho parihīrati;",
  "thag6.14:1.3_0": "Nānākuṇapaparipūro,",
  "thag6.14:1.4_0": "vissavanto tato tato.",
  "thag6.14:2.1_0": "Migaṁ nilīnaṁ kūṭena,",
  "thag6.14:2.2_0": "baḷiseneva ambujaṁ;",
  "thag6.14:2.3_0": "Vānaraṁ viya lepena,",
  "thag6.14:2.4_0": "bādhayanti puthujjanaṁ.",
  "thag6.14:3.1_0": "Rūpā saddā rasā gandhā,",
  "thag6.14:3.2_0": "phoṭṭhabbā ca manoramā;",
  "thag6.14:3.3_0": "Pañca kāmaguṇā ete,",
  "thag6.14:3.4_0": "itthirūpasmi dissare.",
  "thag6.14:4.1_0": "Ye etā upasevanti,",
  "thag6.14:4.2_0": "rattacittā puthujjanā;",
  "thag6.14:4.3_0": "Vaḍḍhenti kaṭasiṁ ghoraṁ,",
  "thag6.14:4.4_0": "ācinanti punabbhavaṁ.",
  "thag6.14:5.1_0": "Yo cetā parivajjeti,",
  "thag6.14:5.2_0": "sappasseva padā siro;",
  "thag6.14:5.3_0": "Somaṁ visattikaṁ loke,",
  "thag6.14:5.4_0": "sato samativattati.",
  "thag6.14:6.1_0": "Kāmesvādīnavaṁ disvā,",
  "thag6.14:6.2_0": "nekkhammaṁ daṭṭhu khemato;",
  "thag6.14:6.3_0": "Nissaṭo sabbakāmehi,",
  "thag6.14:6.4_0": "patto me āsavakkhayo”ti.",
  "thag6.14:6.5_0": "…",
  "thag6.14:6.6_0": "Sabbakāmitthero ….",
  "thag6.14:6.7_0": "Chakkanipāto niṭṭhito.",
  "thag6.14:7.0_0": "Tatruddānaṁ",
  "thag6.14:7.1_0": "Uruveḷakassapo ca,",
  "thag6.14:7.2_0": "thero tekicchakāri ca;",
  "thag6.14:7.3_0": "Mahānāgo ca kullo ca,",
  "thag6.14:7.4_0": "mālukyo sappadāsako;",
  "thag6.14:7.5_0": "Kātiyāno migajālo,",
  "thag6.14:7.6_0": "jento sumanasavhayo;",
  "thag6.14:7.7_0": "Nhātamuni brahmadatto,",
  "thag6.14:7.8_0": "sirimaṇḍo sabbakāmī ca;",
  "thag6.14:7.9_0": "Gāthāyo caturāsīti,",
  "thag6.14:7.10_0": "therā cettha catuddasāti."
}