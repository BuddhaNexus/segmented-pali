{
  "ja71:0.1_0": "Jātaka",
  "ja71:0.2_0": "Ekakanipāta",
  "ja71:0.3_0": "Varuṇavagga",
  "ja71:0.4_0": "1. Varuṇajātaka",
  "ja71:1.1_0": "“Yo pubbe karaṇīyāni,",
  "ja71:1.2_0": "pacchā so kātumicchati;",
  "ja71:1.3_0": "Varuṇakaṭṭhabhañjova,",
  "ja71:1.4_0": "sa pacchā manutappatī”ti.",
  "ja71:2.1_0": "Varuṇajātakaṁ paṭhamaṁ."
}