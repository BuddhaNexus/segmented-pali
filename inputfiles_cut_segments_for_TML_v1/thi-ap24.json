{
  "thi-ap24:0.1_0": "Therīapadāna",
  "thi-ap24:0.2_0": "Kuṇḍalakesīvagga",
  "thi-ap24:0.3_0": "4. Sakulātherīapadāna",
  "thi-ap24:1.1_0": "“Padumuttaro nāma jino,",
  "thi-ap24:1.2_0": "sabbadhammāna pāragū;",
  "thi-ap24:1.3_0": "Ito satasahassamhi,",
  "thi-ap24:1.4_0": "kappe uppajji nāyako.",
  "thi-ap24:2.1_0": "Hitāya sabbasattānaṁ,",
  "thi-ap24:2.2_0": "sukhāya vadataṁ varo;",
  "thi-ap24:2.3_0": "Atthāya purisājañño,",
  "thi-ap24:2.4_0": "paṭipanno sadevake.",
  "thi-ap24:3.1_0": "Yasaggapatto sirimā,",
  "thi-ap24:3.2_0": "kittivaṇṇagato jino;",
  "thi-ap24:3.3_0": "Pūjito sabbalokassa,",
  "thi-ap24:3.4_0": "disāsabbāsu vissuto.",
  "thi-ap24:4.1_0": "Uttiṇṇavicikiccho so,",
  "thi-ap24:4.2_0": "vītivattakathaṅkatho;",
  "thi-ap24:4.3_0": "Sampuṇṇamanasaṅkappo,",
  "thi-ap24:4.4_0": "patto sambodhimuttamaṁ.",
  "thi-ap24:5.1_0": "Anuppannassa maggassa,",
  "thi-ap24:5.2_0": "uppādetā naruttamo;",
  "thi-ap24:5.3_0": "Anakkhātañca akkhāsi,",
  "thi-ap24:5.4_0": "asañjātañca sañjanī.",
  "thi-ap24:6.1_0": "Maggaññū ca maggavidū,",
  "thi-ap24:6.2_0": "maggakkhāyī narāsabho;",
  "thi-ap24:6.3_0": "Maggassa kusalo satthā,",
  "thi-ap24:6.4_0": "sārathīnaṁ varuttamo.",
  "thi-ap24:7.1_0": "Mahākāruṇiko satthā,",
  "thi-ap24:7.2_0": "dhammaṁ desesi nāyako;",
  "thi-ap24:7.3_0": "Nimugge kāmapaṅkamhi,",
  "thi-ap24:7.4_0": "samuddharati pāṇine.",
  "thi-ap24:8.1_0": "Tadāhaṁ haṁsavatiyaṁ,",
  "thi-ap24:8.2_0": "jātā khattiyanandanā;",
  "thi-ap24:8.3_0": "Surūpā sadhanā cāpi,",
  "thi-ap24:8.4_0": "dayitā ca sirīmatī.",
  "thi-ap24:9.1_0": "Ānandassa mahārañño,",
  "thi-ap24:9.2_0": "dhītā paramasobhaṇā;",
  "thi-ap24:9.3_0": "Vemātā bhaginī cāpi,",
  "thi-ap24:9.4_0": "padumuttaranāmino.",
  "thi-ap24:10.1_0": "Rājakaññāhi sahitā,",
  "thi-ap24:10.2_0": "sabbābharaṇabhūsitā;",
  "thi-ap24:10.3_0": "Upāgamma mahāvīraṁ,",
  "thi-ap24:10.4_0": "assosiṁ dhammadesanaṁ.",
  "thi-ap24:11.1_0": "Tadā hi so lokagaru,",
  "thi-ap24:11.2_0": "bhikkhuniṁ dibbacakkhukaṁ;",
  "thi-ap24:11.3_0": "Kittayaṁ parisāmajjhe,",
  "thi-ap24:11.4_0": "aggaṭṭhāne ṭhapesi taṁ.",
  "thi-ap24:12.1_0": "Suṇitvā tamahaṁ haṭṭhā,",
  "thi-ap24:12.2_0": "dānaṁ datvāna satthuno;",
  "thi-ap24:12.3_0": "Pūjitvāna ca sambuddhaṁ,",
  "thi-ap24:12.4_0": "dibbacakkhuṁ apatthayiṁ.",
  "thi-ap24:13.1_0": "Tato avoca maṁ satthā,",
  "thi-ap24:13.2_0": "‘nande lacchasi patthitaṁ;",
  "thi-ap24:13.3_0": "Padīpadhammadānānaṁ,",
  "thi-ap24:13.4_0": "phalametaṁ sunicchitaṁ.",
  "thi-ap24:14.1_0": "Satasahassito kappe,",
  "thi-ap24:14.2_0": "okkākakulasambhavo;",
  "thi-ap24:14.3_0": "Gotamo nāma gottena,",
  "thi-ap24:14.4_0": "satthā loke bhavissati.",
  "thi-ap24:15.1_0": "Tassa dhammesu dāyādā,",
  "thi-ap24:15.2_0": "orasā dhammanimmitā;",
  "thi-ap24:15.3_0": "Sakulā nāma nāmena,",
  "thi-ap24:15.4_0": "hessati satthu sāvikā’.",
  "thi-ap24:16.1_0": "Tena kammena sukatena,",
  "thi-ap24:16.2_0": "cetanāpaṇidhīhi ca;",
  "thi-ap24:16.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "thi-ap24:16.4_0": "tāvatiṁsamagacchahaṁ.",
  "thi-ap24:17.1_0": "Imamhi bhaddake kappe,",
  "thi-ap24:17.2_0": "brahmabandhu mahāyaso;",
  "thi-ap24:17.3_0": "Kassapo nāma gottena,",
  "thi-ap24:17.4_0": "uppajji vadataṁ varo.",
  "thi-ap24:18.1_0": "Paribbājakinī āsiṁ,",
  "thi-ap24:18.2_0": "tadāhaṁ ekacārinī;",
  "thi-ap24:18.3_0": "Bhikkhāya vicaritvāna,",
  "thi-ap24:18.4_0": "alabhiṁ telamattakaṁ.",
  "thi-ap24:19.1_0": "Tena dīpaṁ padīpetvā,",
  "thi-ap24:19.2_0": "upaṭṭhiṁ sabbasaṁvariṁ;",
  "thi-ap24:19.3_0": "Cetiyaṁ dvipadaggassa,",
  "thi-ap24:19.4_0": "vippasannena cetasā.",
  "thi-ap24:20.1_0": "Tena kammena sukatena,",
  "thi-ap24:20.2_0": "cetanāpaṇidhīhi ca;",
  "thi-ap24:20.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "thi-ap24:20.4_0": "tāvatiṁsamagacchahaṁ.",
  "thi-ap24:21.1_0": "Yattha yatthūpapajjāmi,",
  "thi-ap24:21.2_0": "tassa kammassa vāhasā;",
  "thi-ap24:21.3_0": "Pajjalanti mahādīpā,",
  "thi-ap24:21.4_0": "tattha tattha gatāya me.",
  "thi-ap24:22.1_0": "Tirokuṭṭaṁ tiroselaṁ,",
  "thi-ap24:22.2_0": "samatiggayha pabbataṁ;",
  "thi-ap24:22.3_0": "Passāmahaṁ yadicchāmi,",
  "thi-ap24:22.4_0": "dīpadānassidaṁ phalaṁ.",
  "thi-ap24:23.1_0": "Visuddhanayanā homi,",
  "thi-ap24:23.2_0": "yasasā ca jalāmahaṁ;",
  "thi-ap24:23.3_0": "Saddhāpaññāvatī ceva,",
  "thi-ap24:23.4_0": "dīpadānassidaṁ phalaṁ.",
  "thi-ap24:24.1_0": "Pacchime ca bhave dāni,",
  "thi-ap24:24.2_0": "jātā vippakule ahaṁ;",
  "thi-ap24:24.3_0": "Pahūtadhanadhaññamhi,",
  "thi-ap24:24.4_0": "mudite rājapūjite.",
  "thi-ap24:25.1_0": "Ahaṁ sabbaṅgasampannā,",
  "thi-ap24:25.2_0": "sabbābharaṇabhūsitā;",
  "thi-ap24:25.3_0": "Purappavese sugataṁ,",
  "thi-ap24:25.4_0": "vātapāne ṭhitā ahaṁ.",
  "thi-ap24:26.1_0": "Disvā jalantaṁ yasasā,",
  "thi-ap24:26.2_0": "devamanussasakkataṁ;",
  "thi-ap24:26.3_0": "Anubyañjanasampannaṁ,",
  "thi-ap24:26.4_0": "lakkhaṇehi vibhūsitaṁ.",
  "thi-ap24:27.1_0": "Udaggacittā sumanā,",
  "thi-ap24:27.2_0": "pabbajjaṁ samarocayiṁ;",
  "thi-ap24:27.3_0": "Na cireneva kālena,",
  "thi-ap24:27.4_0": "arahattamapāpuṇiṁ.",
  "thi-ap24:28.1_0": "Iddhīsu ca vasī homi,",
  "thi-ap24:28.2_0": "dibbāya sotadhātuyā;",
  "thi-ap24:28.3_0": "Paracittāni jānāmi,",
  "thi-ap24:28.4_0": "satthusāsanakārikā.",
  "thi-ap24:29.1_0": "Pubbenivāsaṁ jānāmi,",
  "thi-ap24:29.2_0": "dibbacakkhu visodhitaṁ;",
  "thi-ap24:29.3_0": "Khepetvā āsave sabbe,",
  "thi-ap24:29.4_0": "visuddhāsiṁ sunimmalā.",
  "thi-ap24:30.1_0": "Pariciṇṇo mayā satthā,",
  "thi-ap24:30.2_0": "kataṁ buddhassa sāsanaṁ;",
  "thi-ap24:30.3_0": "Ohito garuko bhāro,",
  "thi-ap24:30.4_0": "bhavanetti samūhatā.",
  "thi-ap24:31.1_0": "Yassatthāya pabbajitā,",
  "thi-ap24:31.2_0": "agārasmānagāriyaṁ;",
  "thi-ap24:31.3_0": "So me attho anuppatto,",
  "thi-ap24:31.4_0": "sabbasaṁyojanakkhayo.",
  "thi-ap24:32.1_0": "Tato mahākāruṇiko,",
  "thi-ap24:32.2_0": "etadagge ṭhapesi maṁ;",
  "thi-ap24:32.3_0": "Dibbacakkhukānaṁ aggā,",
  "thi-ap24:32.4_0": "sakulāti naruttamo.",
  "thi-ap24:33.1_0": "Kilesā jhāpitā mayhaṁ,",
  "thi-ap24:33.2_0": "…pe…",
  "thi-ap24:33.3_0": "viharāmi anāsavā.",
  "thi-ap24:34.1_0": "Svāgataṁ vata me āsi,",
  "thi-ap24:34.2_0": "…pe…",
  "thi-ap24:34.3_0": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap24:35.1_0": "Paṭisambhidā catasso,",
  "thi-ap24:35.2_0": "…pe…",
  "thi-ap24:35.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "thi-ap24:36.1_0": "Itthaṁ sudaṁ sakulā bhikkhunī imā gāthāyo abhāsitthāti.",
  "thi-ap24:37.1_0": "Sakulātheriyāpadānaṁ catutthaṁ."
}