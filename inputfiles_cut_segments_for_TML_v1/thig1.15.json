{
  "thig1.15:0.1_0": "Therīgāthā",
  "thig1.15:0.2_0": "Ekakanipāta",
  "thig1.15:0.3_0": "15. Uttarātherīgāthā",
  "thig1.15:1.1_0": "“Kāyena saṁvutā āsiṁ,",
  "thig1.15:1.2_0": "vācāya uda cetasā;",
  "thig1.15:1.3_0": "Samūlaṁ taṇhamabbuyha,",
  "thig1.15:1.4_0": "sītibhūtāmhi nibbutā”ti.",
  "thig1.15:2.1_0": "… Uttarā therī …."
}