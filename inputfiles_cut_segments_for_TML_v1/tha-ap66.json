{
  "tha-ap66:0.1_0": "Therāpadāna",
  "tha-ap66:0.2_0": "Sakacintaniyavagga",
  "tha-ap66:0.3_0": "4. Parappasādakattheraapadāna",
  "tha-ap66:1.1_0": "“Usabhaṁ pavaraṁ vīraṁ,",
  "tha-ap66:1.2_0": "Mahesiṁ vijitāvinaṁ;",
  "tha-ap66:1.3_0": "Suvaṇṇavaṇṇaṁ sambuddhaṁ,",
  "tha-ap66:1.4_0": "Ko disvā nappasīdati.",
  "tha-ap66:2.1_0": "Himavāvāparimeyyo,",
  "tha-ap66:2.2_0": "sāgarova duruttaro;",
  "tha-ap66:2.3_0": "Tatheva jhānaṁ buddhassa,",
  "tha-ap66:2.4_0": "ko disvā nappasīdati.",
  "tha-ap66:3.1_0": "Vasudhā yathāppameyyā,",
  "tha-ap66:3.2_0": "cittā vanavaṭaṁsakā;",
  "tha-ap66:3.3_0": "Tatheva sīlaṁ buddhassa,",
  "tha-ap66:3.4_0": "ko disvā nappasīdati.",
  "tha-ap66:4.1_0": "Anilañjasāsaṅkhubbho,",
  "tha-ap66:4.2_0": "yathākāso asaṅkhiyo;",
  "tha-ap66:4.3_0": "Tatheva ñāṇaṁ buddhassa,",
  "tha-ap66:4.4_0": "ko disvā nappasīdati.",
  "tha-ap66:5.1_0": "Imāhi catugāthāhi,",
  "tha-ap66:5.2_0": "brāhmaṇo senasavhayo;",
  "tha-ap66:5.3_0": "Buddhaseṭṭhaṁ thavitvāna,",
  "tha-ap66:5.4_0": "siddhatthaṁ aparājitaṁ.",
  "tha-ap66:6.1_0": "Catunnavutikappāni,",
  "tha-ap66:6.2_0": "duggatiṁ nupapajjatha;",
  "tha-ap66:6.3_0": "Sugatiṁ sukhasampattiṁ,",
  "tha-ap66:6.4_0": "anubhosimanappakaṁ.",
  "tha-ap66:7.1_0": "Catunnavutito kappe,",
  "tha-ap66:7.2_0": "thavitvā lokanāyakaṁ;",
  "tha-ap66:7.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap66:7.4_0": "thomanāya idaṁ phalaṁ.",
  "tha-ap66:8.1_0": "Cātuddasamhi kappamhi,",
  "tha-ap66:8.2_0": "caturo āsumuggatā;",
  "tha-ap66:8.3_0": "Sattaratanasampannā,",
  "tha-ap66:8.4_0": "cakkavattī mahabbalā.",
  "tha-ap66:9.1_0": "Paṭisambhidā catasso,",
  "tha-ap66:9.2_0": "…pe…",
  "tha-ap66:9.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap66:10.1_0": "Itthaṁ sudaṁ āyasmā parappasādako thero imā gāthāyo abhāsitthāti.",
  "tha-ap66:11.1_0": "Parappasādakattherassāpadānaṁ catutthaṁ."
}