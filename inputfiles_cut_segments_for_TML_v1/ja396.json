{
  "ja396:0.1_0": "Jātaka",
  "ja396:0.2_0": "Sattakanipāta",
  "ja396:0.3_0": "Kukkuvagga",
  "ja396:0.4_0": "1. Kukkujātaka",
  "ja396:1.1_0": "“Diyaḍḍhakukkū udayena kaṇṇikā,",
  "ja396:1.2_0": "Vidatthiyo aṭṭha parikkhipanti naṁ;",
  "ja396:1.3_0": "Sā siṁsapā sāramayā apheggukā,",
  "ja396:1.4_0": "Kuhiṁ ṭhitā upparito na dhaṁsati”.",
  "ja396:2.1_0": "“Yā tiṁsati sāramayā anujjukā,",
  "ja396:2.2_0": "Parikiriya gopānasiyo samaṁ ṭhitā;",
  "ja396:2.3_0": "Tāhi susaṅgahitā balasā pīḷitā,",
  "ja396:2.4_0": "Samaṁ ṭhitā upparito na dhaṁsati.",
  "ja396:3.1_0": "Evampi mittehi daḷhehi paṇḍito,",
  "ja396:3.2_0": "Abhejjarūpehi sucīhi mantibhi;",
  "ja396:3.3_0": "Susaṅgahīto siriyā na dhaṁsati,",
  "ja396:3.4_0": "Gopānasī bhāravahāva kaṇṇikā.",
  "ja396:4.1_0": "Kharattacaṁ bellaṁ yathāpi satthavā,",
  "ja396:4.2_0": "Anāmasantopi karoti tittakaṁ;",
  "ja396:4.3_0": "Samāharaṁ sāduṁ karoti patthiva,",
  "ja396:4.4_0": "Asāduṁ kayirā tanubandhamuddharaṁ.",
  "ja396:5.1_0": "Evampi gāmanigamesu paṇḍito,",
  "ja396:5.2_0": "Asāhasaṁ rājadhanāni saṅgharaṁ;",
  "ja396:5.3_0": "Dhammānuvattī paṭipajjamāno,",
  "ja396:5.4_0": "Sa phāti kayirā aviheṭhayaṁ paraṁ.",
  "ja396:6.1_0": "Odātamūlaṁ sucivārisambhavaṁ,",
  "ja396:6.2_0": "Jātaṁ yathā pokkharaṇīsu ambujaṁ;",
  "ja396:6.3_0": "Padumaṁ yathā agginikāsiphālimaṁ,",
  "ja396:6.4_0": "Na kaddamo na rajo na vāri limpati.",
  "ja396:7.1_0": "Evampi vohārasuciṁ asāhasaṁ,",
  "ja396:7.2_0": "Visuddhakammantamapetapāpakaṁ;",
  "ja396:7.3_0": "Na limpati kammakilesa tādiso,",
  "ja396:7.4_0": "Jātaṁ yathā pokkharaṇīsu ambujan”ti.",
  "ja396:8.1_0": "Kukkujātakaṁ paṭhamaṁ."
}