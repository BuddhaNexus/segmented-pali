{
  "ja384:0.1_0": "Jātaka",
  "ja384:0.2_0": "Chakkanipāta",
  "ja384:0.3_0": "Avāriyavagga",
  "ja384:0.4_0": "9. Dhammadhajajātaka",
  "ja384:1.1_0": "“Dhammaṁ caratha ñātayo,",
  "ja384:1.2_0": "Dhammaṁ caratha bhaddaṁ vo;",
  "ja384:1.3_0": "Dhammacārī sukhaṁ seti,",
  "ja384:1.4_0": "Asmiṁ loke paramhi ca”.",
  "ja384:2.1_0": "“Bhaddako vatayaṁ pakkhī,",
  "ja384:2.2_0": "dijo paramadhammiko;",
  "ja384:2.3_0": "Ekapādena tiṭṭhanto,",
  "ja384:2.4_0": "dhammamevānusāsati”.",
  "ja384:3.1_0": "“Nāssa sīlaṁ vijānātha,",
  "ja384:3.2_0": "anaññāya pasaṁsatha;",
  "ja384:3.3_0": "Bhutvā aṇḍañca potañca,",
  "ja384:3.4_0": "dhammo dhammoti bhāsati.",
  "ja384:4.1_0": "Aññaṁ bhaṇati vācāya,",
  "ja384:4.2_0": "aññaṁ kāyena kubbati;",
  "ja384:4.3_0": "Vācāya no ca kāyena,",
  "ja384:4.4_0": "na taṁ dhammaṁ adhiṭṭhito.",
  "ja384:5.1_0": "Vācāya sakhilo manoviduggo,",
  "ja384:5.2_0": "Channo kūpasayova kaṇhasappo;",
  "ja384:5.3_0": "Dhammadhajo gāmanigamāsu sādhu,",
  "ja384:5.4_0": "Dujjāno purisena bālisena.",
  "ja384:6.1_0": "Imaṁ tuṇḍehi pakkhehi,",
  "ja384:6.2_0": "pādā cimaṁ viheṭhatha;",
  "ja384:6.3_0": "Chavañhimaṁ vināsetha,",
  "ja384:6.4_0": "nāyaṁ saṁvāsanāraho”ti.",
  "ja384:7.1_0": "Dhammadhajajātakaṁ navamaṁ."
}