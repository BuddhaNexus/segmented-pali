{
  "pli-tv-bu-vb-sk4:0.1_0": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk4:0.2_0": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk4:0.3_0": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk4:0.4_0": "Parimaṇḍalavagga",
  "pli-tv-bu-vb-sk4:0.5_0": "4. Dutiyasuppaṭicchannasikkhāpada",
  "pli-tv-bu-vb-sk4:1.1_0": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk4:1.2_0": "Tena kho pana samayena chabbaggiyā bhikkhū kāyaṁ vivaritvā antaraghare nisīdanti …pe….",
  "pli-tv-bu-vb-sk4:1.3.1_0": "“Suppaṭicchanno antaraghare nisīdissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk4:1.4_0": "Suppaṭicchannena antaraghare nisīditabbaṁ.",
  "pli-tv-bu-vb-sk4:1.5_0": "Yo anādariyaṁ paṭicca kāyaṁ vivaritvā antaraghare nisīdati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk4:1.6.1_0": "Anāpatti—",
  "pli-tv-bu-vb-sk4:1.7_0": "asañcicca,",
  "pli-tv-bu-vb-sk4:1.8_0": "assatiyā,",
  "pli-tv-bu-vb-sk4:1.9_0": "ajānantassa,",
  "pli-tv-bu-vb-sk4:1.10_0": "gilānassa,",
  "pli-tv-bu-vb-sk4:1.11_0": "vāsūpagatassa,",
  "pli-tv-bu-vb-sk4:1.12_0": "āpadāsu,",
  "pli-tv-bu-vb-sk4:1.13_0": "ummattakassa,",
  "pli-tv-bu-vb-sk4:1.14_0": "ādikammikassāti.",
  "pli-tv-bu-vb-sk4:1.15_0": "Catutthasikkhāpadaṁ niṭṭhitaṁ."
}