{
  "ja206:0.1_0": "Jātaka",
  "ja206:0.2_0": "Dukanipāta",
  "ja206:0.3_0": "Nataṁdaḷhavagga",
  "ja206:0.4_0": "6. Kuruṅgamigajātaka",
  "ja206:1.1_0": "“Iṅgha vaṭṭamayaṁ pāsaṁ,",
  "ja206:1.2_0": "chinda dantehi kacchapa;",
  "ja206:1.3_0": "Ahaṁ tathā karissāmi,",
  "ja206:1.4_0": "yathā nehiti luddako”.",
  "ja206:2.1_0": "“Kacchapo pāvisī vāriṁ,",
  "ja206:2.2_0": "kuruṅgo pāvisī vanaṁ;",
  "ja206:2.3_0": "Satapatto dumaggamhā,",
  "ja206:2.4_0": "dūre putte apānayī”ti.",
  "ja206:3.1_0": "Kuruṅgamigajātakaṁ chaṭṭhaṁ."
}