{
  "snp3.8:0.1_0": "Sutta Nipāta 3.8",
  "snp3.8:0.2_0": "Sallasutta",
  "snp3.8:1.1_0": "Animittamanaññātaṁ,",
  "snp3.8:1.2_0": "maccānaṁ idha jīvitaṁ;",
  "snp3.8:1.3_0": "Kasirañca parittañca,",
  "snp3.8:1.4_0": "tañca dukkhena saṁyutaṁ.",
  "snp3.8:2.1_0": "Na hi so upakkamo atthi,",
  "snp3.8:2.2_0": "yena jātā na miyyare;",
  "snp3.8:2.3_0": "Jarampi patvā maraṇaṁ,",
  "snp3.8:2.4_0": "evaṁdhammā hi pāṇino.",
  "snp3.8:3.1_0": "Phalānamiva pakkānaṁ,",
  "snp3.8:3.2_0": "pāto patanato bhayaṁ;",
  "snp3.8:3.3_0": "Evaṁ jātāna maccānaṁ,",
  "snp3.8:3.4_0": "niccaṁ maraṇato bhayaṁ.",
  "snp3.8:4.1_0": "Yathāpi kumbhakārassa,",
  "snp3.8:4.2_0": "katā mattikabhājanā;",
  "snp3.8:4.3_0": "Sabbe bhedanapariyantā,",
  "snp3.8:4.4_0": "evaṁ maccāna jīvitaṁ.",
  "snp3.8:5.1_0": "Daharā ca mahantā ca,",
  "snp3.8:5.2_0": "ye bālā ye ca paṇḍitā;",
  "snp3.8:5.3_0": "Sabbe maccuvasaṁ yanti,",
  "snp3.8:5.4_0": "sabbe maccuparāyaṇā.",
  "snp3.8:6.1_0": "Tesaṁ maccuparetānaṁ,",
  "snp3.8:6.2_0": "gacchataṁ paralokato;",
  "snp3.8:6.3_0": "Na pitā tāyate puttaṁ,",
  "snp3.8:6.4_0": "ñātī vā pana ñātake.",
  "snp3.8:7.1_0": "Pekkhataṁyeva ñātīnaṁ,",
  "snp3.8:7.2_0": "passa lālapataṁ puthu;",
  "snp3.8:7.3_0": "Ekamekova maccānaṁ,",
  "snp3.8:7.4_0": "govajjho viya nīyati.",
  "snp3.8:8.1_0": "Evamabbhāhato loko,",
  "snp3.8:8.2_0": "Maccunā ca jarāya ca;",
  "snp3.8:8.3_0": "Tasmā dhīrā na socanti,",
  "snp3.8:8.4_0": "Viditvā lokapariyāyaṁ.",
  "snp3.8:9.1_0": "Yassa maggaṁ na jānāsi,",
  "snp3.8:9.2_0": "āgatassa gatassa vā;",
  "snp3.8:9.3_0": "Ubho ante asampassaṁ,",
  "snp3.8:9.4_0": "niratthaṁ paridevasi.",
  "snp3.8:10.1_0": "Paridevayamāno ce,",
  "snp3.8:10.2_0": "Kiñcidatthaṁ udabbahe;",
  "snp3.8:10.3_0": "Sammūḷho hiṁsamattānaṁ,",
  "snp3.8:10.4_0": "Kayirā ce naṁ vicakkhaṇo.",
  "snp3.8:11.1_0": "Na hi ruṇṇena sokena,",
  "snp3.8:11.2_0": "Santiṁ pappoti cetaso;",
  "snp3.8:11.3_0": "Bhiyyassuppajjate dukkhaṁ,",
  "snp3.8:11.4_0": "Sarīraṁ cupahaññati.",
  "snp3.8:12.1_0": "Kiso vivaṇṇo bhavati,",
  "snp3.8:12.2_0": "Hiṁsamattānamattanā;",
  "snp3.8:12.3_0": "Na tena petā pālenti,",
  "snp3.8:12.4_0": "Niratthā paridevanā.",
  "snp3.8:13.1_0": "Sokamappajahaṁ jantu,",
  "snp3.8:13.2_0": "Bhiyyo dukkhaṁ nigacchati;",
  "snp3.8:13.3_0": "Anutthunanto kālaṅkataṁ,",
  "snp3.8:13.4_0": "Sokassa vasamanvagū.",
  "snp3.8:14.1_0": "Aññepi passa gamine,",
  "snp3.8:14.2_0": "Yathākammupage nare;",
  "snp3.8:14.3_0": "Maccuno vasamāgamma,",
  "snp3.8:14.4_0": "Phandantevidha pāṇino.",
  "snp3.8:15.1_0": "Yena yena hi maññanti,",
  "snp3.8:15.2_0": "Tato taṁ hoti aññathā;",
  "snp3.8:15.3_0": "Etādiso vinābhāvo,",
  "snp3.8:15.4_0": "Passa lokassa pariyāyaṁ.",
  "snp3.8:16.1_0": "Api vassasataṁ jīve,",
  "snp3.8:16.2_0": "Bhiyyo vā pana māṇavo;",
  "snp3.8:16.3_0": "Ñātisaṅghā vinā hoti,",
  "snp3.8:16.4_0": "Jahāti idha jīvitaṁ.",
  "snp3.8:17.1_0": "Tasmā arahato sutvā,",
  "snp3.8:17.2_0": "Vineyya paridevitaṁ;",
  "snp3.8:17.3_0": "Petaṁ kālaṅkataṁ disvā,",
  "snp3.8:17.4_0": "Neso labbhā mayā iti.",
  "snp3.8:18.1_0": "Yathā saraṇamādittaṁ,",
  "snp3.8:18.2_0": "Vārinā parinibbaye;",
  "snp3.8:18.3_0": "Evampi dhīro sapañño,",
  "snp3.8:18.4_0": "Paṇḍito kusalo naro;",
  "snp3.8:18.5_0": "Khippamuppatitaṁ sokaṁ,",
  "snp3.8:18.6_0": "Vāto tūlaṁva dhaṁsaye.",
  "snp3.8:19.1_0": "Paridevaṁ pajappañca,",
  "snp3.8:19.2_0": "Domanassañca attano;",
  "snp3.8:19.3_0": "Attano sukhamesāno,",
  "snp3.8:19.4_0": "Abbahe sallamattano.",
  "snp3.8:20.1_0": "Abbūḷhasallo asito,",
  "snp3.8:20.2_0": "santiṁ pappuyya cetaso;",
  "snp3.8:20.3_0": "Sabbasokaṁ atikkanto,",
  "snp3.8:20.4_0": "asoko hoti nibbutoti.",
  "snp3.8:20.5_0": "Sallasuttaṁ aṭṭhamaṁ."
}