{
  "vv43:0.1_0": "Vimānavatthu",
  "vv43:0.2_0": "Itthivimāna",
  "vv43:0.3_0": "Mañjiṭṭhakavagga",
  "vv43:0.4_0": "5. Kañjikadāyikāvimānavatthu",
  "vv43:1.1_0": "“Abhikkantena vaṇṇena,",
  "vv43:1.2_0": "…pe…",
  "vv43:1.3_0": "osadhī viya tārakā.",
  "vv43:2.1_0": "Kena tetādiso vaṇṇo,",
  "vv43:2.2_0": "…pe…",
  "vv43:2.3_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv43:3.1_0": "Sā devatā attamanā,",
  "vv43:3.2_0": "…pe…",
  "vv43:3.3_0": "yassa kammassidaṁ phalaṁ.",
  "vv43:4.1_0": "“Ahaṁ andhakavindamhi,",
  "vv43:4.2_0": "buddhassādiccabandhuno;",
  "vv43:4.3_0": "Adāsiṁ kolasampākaṁ,",
  "vv43:4.4_0": "kañjikaṁ teladhūpitaṁ.",
  "vv43:5.1_0": "Pipphalyā lasuṇena ca,",
  "vv43:5.2_0": "missaṁ lāmañjakena ca;",
  "vv43:5.3_0": "Adāsiṁ ujubhūtasmiṁ,",
  "vv43:5.4_0": "vippasannena cetasā.",
  "vv43:6.1_0": "Yā mahesittaṁ kāreyya,",
  "vv43:6.2_0": "cakkavattissa rājino;",
  "vv43:6.3_0": "Nārī sabbaṅgakalyāṇī,",
  "vv43:6.4_0": "bhattu cānomadassikā;",
  "vv43:6.5_0": "Ekassa kañjikadānassa,",
  "vv43:6.6_0": "kalaṁ nāgghati soḷasiṁ.",
  "vv43:7.1_0": "Sataṁ nikkhā sataṁ assā,",
  "vv43:7.2_0": "sataṁ assatarīrathā;",
  "vv43:7.3_0": "Sataṁ kaññāsahassāni,",
  "vv43:7.4_0": "āmuttamaṇikuṇḍalā;",
  "vv43:7.5_0": "Ekassa kañjikadānassa,",
  "vv43:7.6_0": "kalaṁ nāgghanti soḷasiṁ.",
  "vv43:8.1_0": "Sataṁ hemavatā nāgā,",
  "vv43:8.2_0": "īsādantā urūḷhavā;",
  "vv43:8.3_0": "Suvaṇṇakacchā mātaṅgā,",
  "vv43:8.4_0": "hemakappanavāsasā;",
  "vv43:8.5_0": "Ekassa kañjikadānassa,",
  "vv43:8.6_0": "kalaṁ nāgghanti soḷasiṁ.",
  "vv43:9.1_0": "Catunnamapi dīpānaṁ,",
  "vv43:9.2_0": "issaraṁ yodha kāraye;",
  "vv43:9.3_0": "Ekassa kañjikadānassa,",
  "vv43:9.4_0": "kalaṁ nāgghati soḷasin”ti.",
  "vv43:10.1_0": "Kañjikadāyikāvimānaṁ pañcamaṁ."
}