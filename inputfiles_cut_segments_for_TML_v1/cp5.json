{
  "cp5:0.1_0": "Cariyāpiṭaka",
  "cp5:0.2_0": "Akittivagga",
  "cp5:0.3_0": "Dānapāramī 5",
  "cp5:0.4_0": "Mahāgovindacariya",
  "cp5:1.1_0": "“Punāparaṁ yadā homi,",
  "cp5:1.2_0": "sattarājapurohito;",
  "cp5:1.3_0": "Pūjito naradevehi,",
  "cp5:1.4_0": "mahāgovindabrāhmaṇo.",
  "cp5:2.1_0": "Tadāhaṁ sattarajjesu,",
  "cp5:2.2_0": "Yaṁ me āsi upāyanaṁ;",
  "cp5:2.3_0": "Tena demi mahādānaṁ,",
  "cp5:2.4_0": "Akkhobbhaṁ sāgarūpamaṁ.",
  "cp5:3.1_0": "Na me dessaṁ dhanaṁ dhaññaṁ,",
  "cp5:3.2_0": "Napi natthi nicayo mayi;",
  "cp5:3.3_0": "Sabbaññutaṁ piyaṁ mayhaṁ,",
  "cp5:3.4_0": "Tasmā demi varaṁ dhanan”ti.",
  "cp5:4.1_0": "Mahāgovindacariyaṁ pañcamaṁ."
}