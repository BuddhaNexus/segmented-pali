{
  "tha-ap205:0.1_0": "Therāpadāna",
  "tha-ap205:0.2_0": "Kaṇikārapupphiyavagga",
  "tha-ap205:0.3_0": "3 Kiṅkaṇipupphiyattheraapadāna",
  "tha-ap205:1.1_0": "“Kañcanagghiyasaṅkāso,",
  "tha-ap205:1.2_0": "sabbaññū lokanāyako;",
  "tha-ap205:1.3_0": "Odakaṁ dahamoggayha,",
  "tha-ap205:1.4_0": "sināyi lokanāyako.",
  "tha-ap205:2.1_0": "Paggayha kiṅkaṇiṁ pupphaṁ,",
  "tha-ap205:2.2_0": "Vipassissābhiropayiṁ;",
  "tha-ap205:2.3_0": "Udaggacitto sumano,",
  "tha-ap205:2.4_0": "Dvipadindassa tādino.",
  "tha-ap205:3.1_0": "Ekanavutito kappe,",
  "tha-ap205:3.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap205:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap205:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap205:4.1_0": "Sattavīsatikappamhi,",
  "tha-ap205:4.2_0": "rājā bhīmaratho ahu;",
  "tha-ap205:4.3_0": "Sattaratanasampanno,",
  "tha-ap205:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap205:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap205:5.2_0": "…pe…",
  "tha-ap205:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap205:6.1_0": "Itthaṁ sudaṁ āyasmā kiṅkaṇipupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap205:7.1_0": "Kiṅkaṇipupphiyattherassāpadānaṁ tatiyaṁ."
}