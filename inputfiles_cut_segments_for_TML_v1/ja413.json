{
  "ja413:0.1_0": "Jātaka",
  "ja413:0.2_0": "Sattakanipāta",
  "ja413:0.3_0": "Gandhāravagga",
  "ja413:0.4_0": "8. Dhūmakārijātaka",
  "ja413:1.1_0": "“Rājā apucchi vidhuraṁ,",
  "ja413:1.2_0": "dhammakāmo yudhiṭṭhilo;",
  "ja413:1.3_0": "Api brāhmaṇa jānāsi,",
  "ja413:1.4_0": "ko eko bahu socati”.",
  "ja413:2.1_0": "“Brāhmaṇo ajayūthena,",
  "ja413:2.2_0": "pahūtedho vane vasaṁ;",
  "ja413:2.3_0": "Dhūmaṁ akāsi vāseṭṭho,",
  "ja413:2.4_0": "rattindivamatandito.",
  "ja413:3.1_0": "Tassa taṁ dhūmagandhena,",
  "ja413:3.2_0": "sarabhā makasaḍḍitā;",
  "ja413:3.3_0": "Vassāvāsaṁ upāgacchuṁ,",
  "ja413:3.4_0": "dhūmakārissa santike.",
  "ja413:4.1_0": "Sarabhesu manaṁ katvā,",
  "ja413:4.2_0": "ajā so nāvabujjhatha;",
  "ja413:4.3_0": "Āgacchantī vajantī vā,",
  "ja413:4.4_0": "tassa tā vinasuṁ ajā.",
  "ja413:5.1_0": "Sarabhā sarade kāle,",
  "ja413:5.2_0": "pahīnamakase vane;",
  "ja413:5.3_0": "Pāvisuṁ giriduggāni,",
  "ja413:5.4_0": "nadīnaṁ pabhavāni ca.",
  "ja413:6.1_0": "Sarabhe ca gate disvā,",
  "ja413:6.2_0": "ajā ca vibhavaṁ gatā;",
  "ja413:6.3_0": "Kiso ca vivaṇṇo cāsi,",
  "ja413:6.4_0": "paṇḍurogī ca brāhmaṇo.",
  "ja413:7.1_0": "Evaṁ yo saṁ niraṅkatvā,",
  "ja413:7.2_0": "āgantuṁ kurute piyaṁ;",
  "ja413:7.3_0": "So eko bahu socati,",
  "ja413:7.4_0": "dhūmakārīva brāhmaṇo”ti.",
  "ja413:8.1_0": "Dhūmakārijātakaṁ aṭṭhamaṁ."
}