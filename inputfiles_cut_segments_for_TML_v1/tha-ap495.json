{
  "tha-ap495:0.1_0": "Therāpadāna",
  "tha-ap495:0.2_0": "Kiṅkaṇipupphavagga",
  "tha-ap495:0.3_0": "3. Koraṇḍapupphiyattheraapadāna",
  "tha-ap495:1.1_0": "“Vanakammiko pure āsiṁ,",
  "tha-ap495:1.2_0": "pitumātumatenahaṁ;",
  "tha-ap495:1.3_0": "Pasumārena jīvāmi,",
  "tha-ap495:1.4_0": "kusalaṁ me na vijjati.",
  "tha-ap495:2.1_0": "Mama āsayasāmantā,",
  "tha-ap495:2.2_0": "tisso lokagganāyako;",
  "tha-ap495:2.3_0": "Padāni tīṇi dassesi,",
  "tha-ap495:2.4_0": "anukampāya cakkhumā.",
  "tha-ap495:3.1_0": "Akkante ca pade disvā,",
  "tha-ap495:3.2_0": "tissanāmassa satthuno;",
  "tha-ap495:3.3_0": "Haṭṭho haṭṭhena cittena,",
  "tha-ap495:3.4_0": "pade cittaṁ pasādayiṁ.",
  "tha-ap495:4.1_0": "Koraṇḍaṁ pupphitaṁ disvā,",
  "tha-ap495:4.2_0": "pādapaṁ dharaṇīruhaṁ;",
  "tha-ap495:4.3_0": "Sakosakaṁ gahetvāna,",
  "tha-ap495:4.4_0": "padaseṭṭhamapūjayiṁ.",
  "tha-ap495:5.1_0": "Tena kammena sukatena,",
  "tha-ap495:5.2_0": "cetanāpaṇidhīhi ca;",
  "tha-ap495:5.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "tha-ap495:5.4_0": "tāvatiṁsamagacchahaṁ.",
  "tha-ap495:6.1_0": "Yaṁ yaṁ yonupapajjāmi,",
  "tha-ap495:6.2_0": "devattaṁ atha mānusaṁ;",
  "tha-ap495:6.3_0": "Koraṇḍakachavi homi,",
  "tha-ap495:6.4_0": "suppabhāso bhavāmahaṁ.",
  "tha-ap495:7.1_0": "Dvenavute ito kappe,",
  "tha-ap495:7.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap495:7.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap495:7.4_0": "padapūjāyidaṁ phalaṁ.",
  "tha-ap495:8.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap495:8.2_0": "…pe…",
  "tha-ap495:8.3_0": "viharāmi anāsavo.",
  "tha-ap495:9.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap495:9.2_0": "…pe…",
  "tha-ap495:9.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap495:10.1_0": "Paṭisambhidā catasso,",
  "tha-ap495:10.2_0": "…pe…",
  "tha-ap495:10.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap495:11.1_0": "Itthaṁ sudaṁ āyasmā koraṇḍapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap495:12.1_0": "Koraṇḍapupphiyattherassāpadānaṁ tatiyaṁ."
}