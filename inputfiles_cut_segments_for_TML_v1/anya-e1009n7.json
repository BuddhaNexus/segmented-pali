{
  "anya-e1009n7:0_0": "Cāritta-niddesa",
  "anya-e1009n7:1_0": "100.",
  "anya-e1009n7:2_0": "Attā hi attano nātho,",
  "anya-e1009n7:3_0": "Ko hi nātho paro siyā;",
  "anya-e1009n7:4_0": "Attanā hi sudantena,",
  "anya-e1009n7:5_0": "Nāthaṁ labhati dullabhaṁ.",
  "anya-e1009n7:6_0": "101.",
  "anya-e1009n7:7_0": "Attānañce piyaṁ jaññā,",
  "anya-e1009n7:8_0": "Rakkheyya naṁ surakkhitaṁ;",
  "anya-e1009n7:9_0": "Tiṇṇamaññataraṁ yāmaṁ,",
  "anya-e1009n7:10_0": "Paṭijaggeyya paṇḍito.",
  "anya-e1009n7:11_0": "102.",
  "anya-e1009n7:12_0": "Attanā kurute lakkhiṁ,",
  "anya-e1009n7:13_0": "A-lakkhiṁ kurutettanā;",
  "anya-e1009n7:14_0": "Na hi lakkhiṁ a-lakkhiṁ vā,",
  "anya-e1009n7:15_0": "Añño aññassa kārako.",
  "anya-e1009n7:16_0": "103.",
  "anya-e1009n7:17_0": "Na pīḷitā atta-dukkhena dhīrā,",
  "anya-e1009n7:18_0": "Sukhapphalaṁ kammaṁ pariccajanti;",
  "anya-e1009n7:19_0": "Sammohitāvāpi sukhena mattā,",
  "anya-e1009n7:20_0": "Na pāpa-kammañca samācaranti.",
  "anya-e1009n7:21_0": "104.",
  "anya-e1009n7:22_0": "Yo neva nindaṁ nappasaṁsaṁ,",
  "anya-e1009n7:23_0": "Ādiyati garahaṁ nopi pūjaṁ;",
  "anya-e1009n7:24_0": "Sirīca lakkhīca apeti tamhā,",
  "anya-e1009n7:25_0": "Āpo suvuṭṭhīva yathā thalamhā.",
  "anya-e1009n7:26_0": "105.",
  "anya-e1009n7:27_0": "Sādhu dhammaruci rājā,",
  "anya-e1009n7:28_0": "Sādhu paññāṇavā naro;",
  "anya-e1009n7:29_0": "Sādhu mittānama-ddubbho,",
  "anya-e1009n7:30_0": "Pāpassa a-karaṁ sukhaṁ.",
  "anya-e1009n7:31_0": "106.",
  "anya-e1009n7:32_0": "Kalyāṇakārī kalyāṇaṁ,",
  "anya-e1009n7:33_0": "Pāpakārī ca pāpakaṁ;",
  "anya-e1009n7:34_0": "Yādisaṁ vapatte bījaṁ,",
  "anya-e1009n7:35_0": "Tādisaṁ vahate phalaṁ.",
  "anya-e1009n7:36_0": "107.",
  "anya-e1009n7:37_0": "Jīvaṁ paññāya rakkheyya,",
  "anya-e1009n7:38_0": "Dhanaṁ kammena rakkhaye;",
  "anya-e1009n7:39_0": "Evaṁ hyarogo sukhito,",
  "anya-e1009n7:40_0": "Porāṇaka-vaco idaṁ.",
  "anya-e1009n7:41_0": "108.",
  "anya-e1009n7:42_0": "Anupubbena medhāvī,",
  "anya-e1009n7:43_0": "Thokaṁ thokaṁ khaṇe khaṇe;",
  "anya-e1009n7:44_0": "Kammāro rajatasseva,",
  "anya-e1009n7:45_0": "Niddame malamattano.",
  "anya-e1009n7:46_0": "109.",
  "anya-e1009n7:47_0": "Vācānurakkhī manasā susaṁvuto,",
  "anya-e1009n7:48_0": "Kāyena ca nākusalaṁ kayirā;",
  "anya-e1009n7:49_0": "Etetayo kammapathevisodhaye,",
  "anya-e1009n7:50_0": "Ārādhaye maggamisi-ppaveditaṁ.",
  "anya-e1009n7:51_0": "110.",
  "anya-e1009n7:52_0": "A-sante nopaseveyya,",
  "anya-e1009n7:53_0": "Sante seveyya paṇḍito;",
  "anya-e1009n7:54_0": "A-santo nirayaṁ yanti,",
  "anya-e1009n7:55_0": "Santo pāpenti suggatiṁ.",
  "anya-e1009n7:56_0": "111.",
  "anya-e1009n7:57_0": "A-karontopi ce pāpaṁ,",
  "anya-e1009n7:58_0": "Karontaṁ mupasevati;",
  "anya-e1009n7:59_0": "Saṅkiyo hoti pāpasmiṁ,",
  "anya-e1009n7:60_0": "A-vaṇṇo cassa rūhati.",
  "anya-e1009n7:61_0": "112.",
  "anya-e1009n7:62_0": "Saṅghāgato aniṭṭhehi,",
  "anya-e1009n7:63_0": "Ambopi madhurapphalo;",
  "anya-e1009n7:64_0": "Tittapubbova pā eva,",
  "anya-e1009n7:65_0": "Manusso tu sa-jīvako.",
  "anya-e1009n7:66_0": "113.",
  "anya-e1009n7:67_0": "Nihīna-sevito poso,",
  "anya-e1009n7:68_0": "Nihīyati ca sabbadā;",
  "anya-e1009n7:69_0": "Kadāci na ca hāyetha,",
  "anya-e1009n7:70_0": "Tulyasevīpi attanā.",
  "anya-e1009n7:71_0": "114.",
  "anya-e1009n7:72_0": "Vaṇṇa-gandha-rasopeto ,",
  "anya-e1009n7:73_0": "Amboyaṁ ahuvā pure;",
  "anya-e1009n7:74_0": "Tameva pūjaṁ labhamāno;",
  "anya-e1009n7:75_0": "Kenambo kaṭukapphalo.",
  "anya-e1009n7:76_0": "115.",
  "anya-e1009n7:77_0": "Pucimanda-parivāro ,",
  "anya-e1009n7:78_0": "Ambo te dadhivāhana;",
  "anya-e1009n7:79_0": "Mūlaṁ mūlena saṁsaṭṭhaṁ,",
  "anya-e1009n7:80_0": "Sākhā sākhaṁ nisevare;",
  "anya-e1009n7:81_0": "Asātta-sannivāsena,",
  "anya-e1009n7:82_0": "Tenambo kaṭukapphalo.",
  "anya-e1009n7:83_0": "116.",
  "anya-e1009n7:84_0": "Pāmokkha-bhajanaṁ khippaṁ,",
  "anya-e1009n7:85_0": "Attha-kāmo su-vuḍḍhiyaṁ;",
  "anya-e1009n7:86_0": "Bhaje uttari attanā,",
  "anya-e1009n7:87_0": "Tasmā udeti paṇḍito.",
  "anya-e1009n7:88_0": "117.",
  "anya-e1009n7:89_0": "Kāco kañcana-saṁsaggā,",
  "anya-e1009n7:90_0": "Dhatte māra-katiṁ jutiṁ;",
  "anya-e1009n7:91_0": "Tathā saṁsannidhānena,",
  "anya-e1009n7:92_0": "Mūḷho yāti pavīṇataṁ.",
  "anya-e1009n7:93_0": "118.",
  "anya-e1009n7:94_0": "Kīṭopi sumano-saṅgā,",
  "anya-e1009n7:95_0": "Ārohati sataṁ siro;",
  "anya-e1009n7:96_0": "Asmāpi yāti devatvaṁ,",
  "anya-e1009n7:97_0": "Mahabbhi suppatiṭṭhito.",
  "anya-e1009n7:98_0": "119.",
  "anya-e1009n7:99_0": "Sabbhireva samāsetha,",
  "anya-e1009n7:100_0": "Sabbhi kubbetha sandhavaṁ;",
  "anya-e1009n7:101_0": "Sataṁ saddhammamaññāya,",
  "anya-e1009n7:102_0": "Paññaṁ labhati nāññato.",
  "anya-e1009n7:103_0": "120.",
  "anya-e1009n7:104_0": "Sabbhireva samāsetha,",
  "anya-e1009n7:105_0": "Sabbhi kubbetha sandhavaṁ;",
  "anya-e1009n7:106_0": "Sataṁ saddhammamaññāya,",
  "anya-e1009n7:107_0": "Sabba-dukkhā pamuccatti.",
  "anya-e1009n7:108_0": "121.",
  "anya-e1009n7:109_0": "Sakiṁdeva kulaputta,",
  "anya-e1009n7:110_0": "Sabbhi hoti samāgamo;",
  "anya-e1009n7:111_0": "Sā naṁ saṅgati pāleti,",
  "anya-e1009n7:112_0": "Nāsabbhi bahu saṅgamo.",
  "anya-e1009n7:113_0": "122.",
  "anya-e1009n7:114_0": "Seyyo a-mitto medhāvī,",
  "anya-e1009n7:115_0": "Yañce bālānukampako.",
  "anya-e1009n7:116_0": "123.",
  "anya-e1009n7:117_0": "Sīlavantaṁ paññavantaṁ,",
  "anya-e1009n7:118_0": "Divā nissaya-dāyakaṁ;",
  "anya-e1009n7:119_0": "Bahussutaṁ gavesanto,",
  "anya-e1009n7:120_0": "Bhajeyya attha-māmako.",
  "anya-e1009n7:121_0": "124.",
  "anya-e1009n7:122_0": "Pāpa-mitte vivajjetvā,",
  "anya-e1009n7:123_0": "Bhajeyyuttama-puggalaṁ;",
  "anya-e1009n7:124_0": "Ovāde cassa tiṭṭheyya,",
  "anya-e1009n7:125_0": "Patthento a-calaṁ sukhaṁ.",
  "anya-e1009n7:126_0": "125.",
  "anya-e1009n7:127_0": "Na bhaje pāpake mitte,",
  "anya-e1009n7:128_0": "Na bhaje purisādhame;",
  "anya-e1009n7:129_0": "Bhajetha mittte kalyāṇe,",
  "anya-e1009n7:130_0": "Bhajetha purisuttame.",
  "anya-e1009n7:131_0": "126.",
  "anya-e1009n7:132_0": "Anavajjaṁ mukhamboja,",
  "anya-e1009n7:133_0": "Manavajjā ca bhāratī;",
  "anya-e1009n7:134_0": "Alaṅkatāva sobhante,",
  "anya-e1009n7:135_0": "Kiṁsu te niralaṅkatā.",
  "anya-e1009n7:136_0": "127.",
  "anya-e1009n7:137_0": "Na hi vaṇṇena sampannā,",
  "anya-e1009n7:138_0": "Mañjukā piya-dassinā;",
  "anya-e1009n7:139_0": "Kharā vācā piyā hoti,",
  "anya-e1009n7:140_0": "Asmiṁ loke paramhi ca.",
  "anya-e1009n7:141_0": "128.",
  "anya-e1009n7:142_0": "Nanu passasimaṁ kāḷiṁ,",
  "anya-e1009n7:143_0": "Dubbaṇṇaṁ tilakāhataṁ;",
  "anya-e1009n7:144_0": "Kolilaṁ saṇha-vācena,",
  "anya-e1009n7:145_0": "Bahūnaṁ pāṇinaṁ piyaṁ.",
  "anya-e1009n7:146_0": "129.",
  "anya-e1009n7:147_0": "Tasmā sakhila-vācāya,",
  "anya-e1009n7:148_0": "Manta-bhāṇī anuddhato;",
  "anya-e1009n7:149_0": "Atthaṁ dhammañca dīpeti,",
  "anya-e1009n7:150_0": "Madhuraṁ tassa bhāsitaṁ.",
  "anya-e1009n7:151_0": "130.",
  "anya-e1009n7:152_0": "Tameva vācaṁ bhāseyya,",
  "anya-e1009n7:153_0": "Yā sattānaṁ na tāpaye;",
  "anya-e1009n7:154_0": "Pare ca na vihiṁseyya,",
  "anya-e1009n7:155_0": "Sā ve vācā subhāsitā.",
  "anya-e1009n7:156_0": "131.",
  "anya-e1009n7:157_0": "Piyaṁ vācaṁva bhāseyya,",
  "anya-e1009n7:158_0": "Yā vācā paṭinanditā;",
  "anya-e1009n7:159_0": "Yaṁ anādāya pāpāni;",
  "anya-e1009n7:160_0": "Paresaṁ bhāsate piyaṁ.",
  "anya-e1009n7:161_0": "132.",
  "anya-e1009n7:162_0": "Saccaṁ ve amatā vācā,",
  "anya-e1009n7:163_0": "Esa dhammo sanantano;",
  "anya-e1009n7:164_0": "Sacce atthe ca dhamme ca,",
  "anya-e1009n7:165_0": "Āhu santo patiṭṭhitā.",
  "anya-e1009n7:166_0": "133.",
  "anya-e1009n7:167_0": "Subhāsitañca dhammañca,",
  "anya-e1009n7:168_0": "Piyañca saccameva ca;",
  "anya-e1009n7:169_0": "Catu-aṅgehi sampannaṁ,",
  "anya-e1009n7:170_0": "Vācaṁ bhāseyya paṇḍito.",
  "anya-e1009n7:171_0": "134.",
  "anya-e1009n7:172_0": "Manāpameva bhāseyya,",
  "anya-e1009n7:173_0": "Nāmanāpaṁ kudācanaṁ;",
  "anya-e1009n7:174_0": "Manāpaṁ bhāsamānassa,",
  "anya-e1009n7:175_0": "Siddhaṁ piyosadhaṁ bhave.",
  "anya-e1009n7:176_0": "135.",
  "anya-e1009n7:177_0": "Yaṁ vadeyya taṁ kareyya,",
  "anya-e1009n7:178_0": "Yaṁ na vade na taṁ kare;",
  "anya-e1009n7:179_0": "A-karontaṁ bhāsamānaṁ,",
  "anya-e1009n7:180_0": "Parijānanti paṇḍitā.",
  "anya-e1009n7:181_0": "136.",
  "anya-e1009n7:182_0": "Rahovādaṁ na bhāseyya,",
  "anya-e1009n7:183_0": "Na sammukhā khiṇaṁ bhaṇe;",
  "anya-e1009n7:184_0": "A-taramānova bhaṇeyya,",
  "anya-e1009n7:185_0": "Taramānova no bhaṇe.",
  "anya-e1009n7:186_0": "137.",
  "anya-e1009n7:187_0": "Māvoca pharusaṁ kiñci,",
  "anya-e1009n7:188_0": "Vuttā paṭivadeyyuṁ taṁ;",
  "anya-e1009n7:189_0": "Dukkhā hi sārambhakathā,",
  "anya-e1009n7:190_0": "Paṭidaṇḍā phuseyyuṁ taṁ.",
  "anya-e1009n7:191_0": "138.",
  "anya-e1009n7:192_0": "Saka-yuttaṁ kare kammaṁ,",
  "anya-e1009n7:193_0": "Saka-yuttaṁ vaciṁ bhaṇe;",
  "anya-e1009n7:194_0": "A-yuttake dhanaṁ naṭṭhaṁ,",
  "anya-e1009n7:195_0": "A-yutte jīvitaṁ khaye.",
  "anya-e1009n7:196_0": "139.",
  "anya-e1009n7:197_0": "Ye vuḍḍhamapacāyanti,",
  "anya-e1009n7:198_0": "Narā dhammassa dhakāvidā;",
  "anya-e1009n7:199_0": "Diṭṭhe dhammeva pāsaṁsā,",
  "anya-e1009n7:200_0": "Samparāye ca suggatiṁ.",
  "anya-e1009n7:201_0": "140.",
  "anya-e1009n7:202_0": "Porī-kathaṁ-va bhāseyya,",
  "anya-e1009n7:203_0": "Yuttā kathā hi pūrino;",
  "anya-e1009n7:204_0": "Bhāti-mattañca bhātāti,",
  "anya-e1009n7:205_0": "Pitu-mattaṁ pitā iti.",
  "anya-e1009n7:206_0": "141.",
  "anya-e1009n7:207_0": "Dānañca piya-vajjañca,",
  "anya-e1009n7:208_0": "Attha-cariyā ca yā idha;",
  "anya-e1009n7:209_0": "Samānattatā dhammesa,",
  "anya-e1009n7:210_0": "Tattha tattha yathārahaṁ.",
  "anya-e1009n7:211_0": "142.",
  "anya-e1009n7:212_0": "Sakiṁ vadanti rājāno,",
  "anya-e1009n7:213_0": "Sakiṁṁ samaṇa-brāhmaṇā;",
  "anya-e1009n7:214_0": "Sakiṁva purisā loke,",
  "anya-e1009n7:215_0": "Esadhammo sanantano.",
  "anya-e1009n7:216_0": "143.",
  "anya-e1009n7:217_0": "Eka-vācaṁva dvevācaṁ,",
  "anya-e1009n7:218_0": "Bhaṇeyya anukampako;",
  "anya-e1009n7:219_0": "Taduttari na bhāseyya,",
  "anya-e1009n7:220_0": "Dāsovayyassa santike.",
  "anya-e1009n7:221_0": "144.",
  "anya-e1009n7:222_0": "Tasseva tena pāpiyo,",
  "anya-e1009n7:223_0": "Yo kuddhaṁ paṭikujjhatti;",
  "anya-e1009n7:224_0": "Kuddhaṁ a-paṭikujjhanto,",
  "anya-e1009n7:225_0": "Saṅgāmaṁ jeti dujjayaṁ.",
  "anya-e1009n7:226_0": "145.",
  "anya-e1009n7:227_0": "Ubhinnamatthaṁ carati,",
  "anya-e1009n7:228_0": "Attano ca parassa ca;",
  "anya-e1009n7:229_0": "Paraṁ saṁkupitaṁ ñatvā,",
  "anya-e1009n7:230_0": "Yo sato upasammati.",
  "anya-e1009n7:231_0": "Attānaṁ rakkhanto paraṁ rakkhati,",
  "anya-e1009n7:232_0": "Paraṁ rakkhanto attānaṁ rakkhati.",
  "anya-e1009n7:233_0": "146.",
  "anya-e1009n7:234_0": "Sīla-samādhi-paññānaṁ ,",
  "anya-e1009n7:235_0": "Khantī padhāna-kāraṇaṁ;",
  "anya-e1009n7:236_0": "Sabbepi kusalā dhammā,",
  "anya-e1009n7:237_0": "Khantyāyattāva vaḍḍhare.",
  "anya-e1009n7:238_0": "147.",
  "anya-e1009n7:239_0": "Khamā-khagga-karetassa ,",
  "anya-e1009n7:240_0": "Dujjano kiṁ karissati;",
  "anya-e1009n7:241_0": "A-tiṇe patito vahni,",
  "anya-e1009n7:242_0": "Sayamevūpasammati.",
  "anya-e1009n7:243_0": "148.",
  "anya-e1009n7:244_0": "Sussusā savaṇañceva,",
  "anya-e1009n7:245_0": "Gahaṇaṁ dhāraṇaṁ tathā;",
  "anya-e1009n7:246_0": "Uhāpohatthaviññāṇaṁ,",
  "anya-e1009n7:247_0": "Tatva-ññāṇañcadhīguṇaṁ.",
  "anya-e1009n7:248_0": "149.",
  "anya-e1009n7:249_0": "Yassete caturo dhammā,",
  "anya-e1009n7:250_0": "Atthi posesu paṇḍita;",
  "anya-e1009n7:251_0": "Saccaṁ dhammo dhīti cāgo,",
  "anya-e1009n7:252_0": "Diṭṭhaṁ so ativattati.",
  "anya-e1009n7:253_0": "150.",
  "anya-e1009n7:254_0": "Vejjo purohito mantī,",
  "anya-e1009n7:255_0": "Vedaññotra catutthako;",
  "anya-e1009n7:256_0": "Pabhāta-kāle daṭṭhabbā,",
  "anya-e1009n7:257_0": "Niccaṁ sva-sīrimicchatā.",
  "anya-e1009n7:258_0": "151.",
  "anya-e1009n7:259_0": "Mittānaṁ santikaṁ gacche,",
  "anya-e1009n7:260_0": "Kāle na rattiyaṁ kisaṁ;",
  "anya-e1009n7:261_0": "Ce bahuṁ bhijje cinitvā,",
  "anya-e1009n7:262_0": "Taṁ mittesu samākare.",
  "anya-e1009n7:263_0": "152.",
  "anya-e1009n7:264_0": "Na paresaṁ vilomāni,",
  "anya-e1009n7:265_0": "Na paresaṁ katākataṁ;",
  "anya-e1009n7:266_0": "Attanova avekkheyya,",
  "anya-e1009n7:267_0": "Katāni a-katāni ca.",
  "anya-e1009n7:268_0": "153.",
  "anya-e1009n7:269_0": "Desa-jāti-pubbe-carā ,",
  "anya-e1009n7:270_0": "Anu-carā jano kare;",
  "anya-e1009n7:271_0": "Paresaṁ vedhakaṁ māyaṁ,",
  "anya-e1009n7:272_0": "Taṁ jānitvā sakhaṁ kare.",
  "anya-e1009n7:273_0": "154.",
  "anya-e1009n7:274_0": "Parittaṁ dārumāruyha,",
  "anya-e1009n7:275_0": "Yathā sīde mahaṇṇave;",
  "anya-e1009n7:276_0": "Evaṁ kusītamāgamma,",
  "anya-e1009n7:277_0": "Sādhu-jīvīpi sīdati;",
  "anya-e1009n7:278_0": "Tasmā taṁ parivajjeyya,",
  "anya-e1009n7:279_0": "Kusītaṁ hīna-vīriyaṁ.",
  "anya-e1009n7:280_0": "155.",
  "anya-e1009n7:281_0": "Alasañca pamādo ca,",
  "anya-e1009n7:282_0": "Anuṭṭhānaṁ a-saṁyamo;",
  "anya-e1009n7:283_0": "Niddā tandi ca te chidde,",
  "anya-e1009n7:284_0": "Sabbaso taṁ vivajjaye.",
  "anya-e1009n7:285_0": "156.",
  "anya-e1009n7:286_0": "Cajeyya dummittaṁ bālaṁ,",
  "anya-e1009n7:287_0": "Āsīvisaṁva māṇavo;",
  "anya-e1009n7:288_0": "Bhañjeyya pāpakaṁ kammaṁ,",
  "anya-e1009n7:289_0": "Naḷāgāraṁva kuñjaro.",
  "anya-e1009n7:290_0": "157.",
  "anya-e1009n7:291_0": "Na hi aññañña-cittānaṁ,",
  "anya-e1009n7:292_0": "Itthīnaṁ purisāna vā;",
  "anya-e1009n7:293_0": "Nānāvīkatvā saṁsaggaṁ,",
  "anya-e1009n7:294_0": "Tādisaṁ pica nāsmase.",
  "anya-e1009n7:295_0": "158.",
  "anya-e1009n7:296_0": "Nāsmase kata-pāpamhi,",
  "anya-e1009n7:297_0": "Nāsmase alika-vādine;",
  "anya-e1009n7:298_0": "Nāsmate attatthapaññamhi,",
  "anya-e1009n7:299_0": "Atta-santepi nāsmate.",
  "anya-e1009n7:300_0": "159.",
  "anya-e1009n7:301_0": "Ghatāsanaṁ kuñjaraṁ kaṇha-sappaṁ,",
  "anya-e1009n7:302_0": "Muddhā-bhisittaṁ pamadā ca sabbā;",
  "anya-e1009n7:303_0": "Ete naro niccasato bhajetha,",
  "anya-e1009n7:304_0": "Tesaṁ have dubbidū sabba-bhāvo.",
  "anya-e1009n7:305_0": "160.",
  "anya-e1009n7:306_0": "Itthīnaṁ dujjanāna-ñca,",
  "anya-e1009n7:307_0": "Vissāso no-pa pajjate;",
  "anya-e1009n7:308_0": "Vise siṅgimhi nadiyaṁ,",
  "anya-e1009n7:309_0": "Roge rāja-kulamhi ca.",
  "anya-e1009n7:310_0": "161.",
  "anya-e1009n7:311_0": "Itthi-dhutto surā-dhutto,",
  "anya-e1009n7:312_0": "Akkha-dhutto ca yo naro;",
  "anya-e1009n7:313_0": "Laddhaṁ laddhaṁ vināseti,",
  "anya-e1009n7:314_0": "Taṁ parābhavato mukhaṁ.",
  "anya-e1009n7:315_0": "162.",
  "anya-e1009n7:316_0": "Pāpa-mitto pāpa-sakho,",
  "anya-e1009n7:317_0": "Pāpa-ācāra gocaro;",
  "anya-e1009n7:318_0": "Asmā lokā paramhā ca,",
  "anya-e1009n7:319_0": "Ubhayā dhaṁsate naro.",
  "anya-e1009n7:320_0": "163.",
  "anya-e1009n7:321_0": "Maccherena yasaṁ hataṁ,",
  "anya-e1009n7:322_0": "Kuppanena guṇo hato;",
  "anya-e1009n7:323_0": "Kūṭena nassate saccaṁ,",
  "anya-e1009n7:324_0": "Khuddena dhamma-rakkhanaṁ.",
  "anya-e1009n7:325_0": "164.",
  "anya-e1009n7:326_0": "Akkha-devī dhanāni ca,",
  "anya-e1009n7:327_0": "Vināso hoti āpadā;",
  "anya-e1009n7:328_0": "Ṭhiti hatā pamādo ca,",
  "anya-e1009n7:329_0": "Dvijaṁ bhikkhuñca nassati.",
  "anya-e1009n7:330_0": "165.",
  "anya-e1009n7:331_0": "Pesuññena kulaṁ hataṁ,",
  "anya-e1009n7:332_0": "Mānena hitamattano;",
  "anya-e1009n7:333_0": "Duccaritena mānuso,",
  "anya-e1009n7:334_0": "Daliddāyādaro hato.",
  "anya-e1009n7:335_0": "166.",
  "anya-e1009n7:336_0": "A-mānanā yattha siyā,",
  "anya-e1009n7:337_0": "Santānaṁpi vimānanā;",
  "anya-e1009n7:338_0": "Hīna-sammānanāvāpi,",
  "anya-e1009n7:339_0": "Na tattha vasatiṁ vase.",
  "anya-e1009n7:340_0": "167.",
  "anya-e1009n7:341_0": "Yatthālaso ca dakkho ca,",
  "anya-e1009n7:342_0": "Sūro bhīru ca pūjiyā;",
  "anya-e1009n7:343_0": "Na tattha santo vasanti,",
  "anya-e1009n7:344_0": "A-visesa-kare nare.",
  "anya-e1009n7:345_0": "168.",
  "anya-e1009n7:346_0": "No ce assa sakā-buddhi,",
  "anya-e1009n7:347_0": "Vinayo vā su-sikkhito;",
  "anya-e1009n7:348_0": "Vane andha-mahiṁsova,",
  "anya-e1009n7:349_0": "Careyya bahuko jano.",
  "anya-e1009n7:350_0": "169.",
  "anya-e1009n7:351_0": "Phalaṁ ve kadaliṁ hanti,",
  "anya-e1009n7:352_0": "Phalaṁ veḷuṁ phalaṁ naḷaṁ;",
  "anya-e1009n7:353_0": "Sakkāro kā-purisaṁ hanti,",
  "anya-e1009n7:354_0": "Gabbho assatariṁ yathā.",
  "anya-e1009n7:355_0": "170.",
  "anya-e1009n7:356_0": "Vajjañca vajjato ñatvā,",
  "anya-e1009n7:357_0": "A-vajjañca a-vajjato;",
  "anya-e1009n7:358_0": "Sammādiṭṭhi-samādānā,",
  "anya-e1009n7:359_0": "Sattā gacchantti suggatiṁ."
}