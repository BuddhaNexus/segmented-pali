{
  "tha-ap244:0.1_0": "Therāpadāna",
  "tha-ap244:0.2_0": "Tuvaradāyakavagga",
  "tha-ap244:0.3_0": "2. Nāgakesariyattheraapadāna",
  "tha-ap244:1.1_0": "“Dhanuṁ advejjhaṁ katvāna,",
  "tha-ap244:1.2_0": "vanamajjhogahiṁ ahaṁ;",
  "tha-ap244:1.3_0": "Kesaraṁ ogataṁ disvā,",
  "tha-ap244:1.4_0": "patapattaṁ samuṭṭhitaṁ.",
  "tha-ap244:2.1_0": "Ubho hatthehi paggayha,",
  "tha-ap244:2.2_0": "sire katvāna añjaliṁ;",
  "tha-ap244:2.3_0": "Buddhassa abhiropesiṁ,",
  "tha-ap244:2.4_0": "tissassa lokabandhuno.",
  "tha-ap244:3.1_0": "Dvenavute ito kappe,",
  "tha-ap244:3.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap244:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap244:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap244:4.1_0": "Tesattatimhi kappamhi,",
  "tha-ap244:4.2_0": "satta kesaranāmakā;",
  "tha-ap244:4.3_0": "Sattaratanasampannā,",
  "tha-ap244:4.4_0": "cakkavattī mahabbalā.",
  "tha-ap244:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap244:5.2_0": "…pe…",
  "tha-ap244:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap244:6.1_0": "Itthaṁ sudaṁ āyasmā nāgakesariyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap244:7.1_0": "Nāgakesariyattherassāpadānaṁ dutiyaṁ."
}