{
  "thig11.1:0.1_0": "Therīgāthā",
  "thig11.1:0.2_0": "Dvādasakanipāta",
  "thig11.1:0.3_0": "1. Uppalavaṇṇātherīgāthā",
  "thig11.1:1.1_0": "“Ubho mātā ca dhītā ca,",
  "thig11.1:1.2_0": "mayaṁ āsuṁ sapattiyo;",
  "thig11.1:1.3_0": "Tassā me ahu saṁvego,",
  "thig11.1:1.4_0": "abbhuto lomahaṁsano.",
  "thig11.1:2.1_0": "Dhiratthu kāmā asucī,",
  "thig11.1:2.2_0": "duggandhā bahukaṇṭakā;",
  "thig11.1:2.3_0": "Yattha mātā ca dhītā ca,",
  "thig11.1:2.4_0": "sabhariyā mayaṁ ahuṁ.",
  "thig11.1:3.1_0": "Kāmesvādīnavaṁ disvā,",
  "thig11.1:3.2_0": "nekkhammaṁ daṭṭhu khemato;",
  "thig11.1:3.3_0": "Sā pabbajjiṁ rājagahe,",
  "thig11.1:3.4_0": "agārasmānagāriyaṁ.",
  "thig11.1:4.1_0": "Pubbenivāsaṁ jānāmi,",
  "thig11.1:4.2_0": "dibbacakkhu visodhitaṁ;",
  "thig11.1:4.3_0": "Cetopariccañāṇañca,",
  "thig11.1:4.4_0": "sotadhātu visodhitā.",
  "thig11.1:5.1_0": "Iddhīpi me sacchikatā,",
  "thig11.1:5.2_0": "patto me āsavakkhayo;",
  "thig11.1:5.3_0": "Chaḷabhiññā sacchikatā,",
  "thig11.1:5.4_0": "kataṁ buddhassa sāsanaṁ.",
  "thig11.1:6.1_0": "Iddhiyā abhinimmitvā,",
  "thig11.1:6.2_0": "caturassaṁ rathaṁ ahaṁ;",
  "thig11.1:6.3_0": "Buddhassa pāde vanditvā,",
  "thig11.1:6.4_0": "lokanāthassa tādino”.",
  "thig11.1:7.1_0": "“Supupphitaggaṁ upagamma pādapaṁ,",
  "thig11.1:7.2_0": "Ekā tuvaṁ tiṭṭhasi sālamūle;",
  "thig11.1:7.3_0": "Na cāpi te dutiyo atthi koci,",
  "thig11.1:7.4_0": "Bāle na tvaṁ bhāyasi dhuttakānaṁ”.",
  "thig11.1:8.1_0": "“Sataṁ sahassānipi dhuttakānaṁ,",
  "thig11.1:8.2_0": "Samāgatā edisakā bhaveyyuṁ;",
  "thig11.1:8.3_0": "Lomaṁ na iñje napi sampavedhe,",
  "thig11.1:8.4_0": "Kiṁ me tuvaṁ māra karissaseko.",
  "thig11.1:9.1_0": "Esā antaradhāyāmi,",
  "thig11.1:9.2_0": "kucchiṁ vā pavisāmi te;",
  "thig11.1:9.3_0": "Bhamukantare tiṭṭhāmi,",
  "thig11.1:9.4_0": "tiṭṭhantiṁ maṁ na dakkhasi.",
  "thig11.1:10.1_0": "Cittamhi vasībhūtāhaṁ,",
  "thig11.1:10.2_0": "iddhipādā subhāvitā;",
  "thig11.1:10.3_0": "Chaḷabhiññā sacchikatā,",
  "thig11.1:10.4_0": "kataṁ buddhassa sāsanaṁ.",
  "thig11.1:11.1_0": "Sattisūlūpamā kāmā,",
  "thig11.1:11.2_0": "khandhāsaṁ adhikuṭṭanā;",
  "thig11.1:11.3_0": "Yaṁ tvaṁ ‘kāmaratiṁ’ brūsi,",
  "thig11.1:11.4_0": "‘āratī’ dāni sā mama.",
  "thig11.1:12.1_0": "Sabbattha vihatā nandī,",
  "thig11.1:12.2_0": "Tamokhandho padālito;",
  "thig11.1:12.3_0": "Evaṁ jānāhi pāpima,",
  "thig11.1:12.4_0": "Nihato tvamasi antakā”ti.",
  "thig11.1:13.1_0": "…",
  "thig11.1:13.2_0": "Uppalavaṇṇā therī ….",
  "thig11.1:14.1_0": "Dvādasakanipāto niṭṭhito."
}