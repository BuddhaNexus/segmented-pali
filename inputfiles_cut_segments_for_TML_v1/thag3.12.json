{
  "thag3.12:0.1_0": "Theragāthā",
  "thag3.12:0.2_0": "Tikanipāta",
  "thag3.12:0.3_0": "Paṭhamavagga",
  "thag3.12:0.4_0": "3.12. Uttarapālattheragāthā",
  "thag3.12:1.1_0": "“Paṇḍitaṁ vata maṁ santaṁ,",
  "thag3.12:1.2_0": "alamatthavicintakaṁ;",
  "thag3.12:1.3_0": "Pañca kāmaguṇā loke,",
  "thag3.12:1.4_0": "sammohā pātayiṁsu maṁ.",
  "thag3.12:2.1_0": "Pakkhando māravisaye,",
  "thag3.12:2.2_0": "daḷhasallasamappito;",
  "thag3.12:2.3_0": "Asakkhiṁ maccurājassa,",
  "thag3.12:2.4_0": "ahaṁ pāsā pamuccituṁ.",
  "thag3.12:3.1_0": "Sabbe kāmā pahīnā me,",
  "thag3.12:3.2_0": "bhavā sabbe padālitā;",
  "thag3.12:3.3_0": "Vikkhīṇo jātisaṁsāro,",
  "thag3.12:3.4_0": "natthi dāni punabbhavo”ti.",
  "thag3.12:3.5_0": "… Uttarapālo thero …."
}