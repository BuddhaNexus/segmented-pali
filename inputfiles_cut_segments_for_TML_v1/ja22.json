{
  "ja22:0.1_0": "Jātaka",
  "ja22:0.2_0": "Ekakanipāta",
  "ja22:0.3_0": "Kuruṅgavagga",
  "ja22:0.4_0": "2. Kukkurajātaka",
  "ja22:1.1_0": "“Ye kukkurā rājakulamhi vaddhā,",
  "ja22:1.2_0": "Koleyyakā vaṇṇabalūpapannā;",
  "ja22:1.3_0": "Teme na vajjhā mayamasma vajjhā,",
  "ja22:1.4_0": "Nāyaṁ saghaccā dubbalaghātikāyan”ti.",
  "ja22:2.1_0": "Kukkurajātakaṁ dutiyaṁ."
}