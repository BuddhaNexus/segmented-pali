{
  "tha-ap337:0.1_0": "Therāpadāna",
  "tha-ap337:0.2_0": "Gandhodakavagga",
  "tha-ap337:0.3_0": "5 Phusitakampiyattheraapadāna",
  "tha-ap337:1.1_0": "“Vipassī nāma sambuddho,",
  "tha-ap337:1.2_0": "lokajeṭṭho narāsabho;",
  "tha-ap337:1.3_0": "Khīṇāsavehi sahito,",
  "tha-ap337:1.4_0": "saṅghārāme vasī tadā.",
  "tha-ap337:2.1_0": "Ārāmadvārā nikkhamma,",
  "tha-ap337:2.2_0": "vipassī lokanāyako;",
  "tha-ap337:2.3_0": "Saha satasahassehi,",
  "tha-ap337:2.4_0": "aṭṭha khīṇāsavehi so.",
  "tha-ap337:3.1_0": "Ajinena nivatthohaṁ,",
  "tha-ap337:3.2_0": "vākacīradharopi ca;",
  "tha-ap337:3.3_0": "Kusumodakamādāya,",
  "tha-ap337:3.4_0": "sambuddhaṁ upasaṅkamiṁ.",
  "tha-ap337:4.1_0": "Sakaṁ cittaṁ pasādetvā,",
  "tha-ap337:4.2_0": "vedajāto katañjalī;",
  "tha-ap337:4.3_0": "Kusumodakamādāya,",
  "tha-ap337:4.4_0": "buddhamabbhukkiriṁ ahaṁ.",
  "tha-ap337:5.1_0": "Tena kammena sambuddho,",
  "tha-ap337:5.2_0": "jalajuttamanāmako;",
  "tha-ap337:5.3_0": "Mama kammaṁ pakittetvā,",
  "tha-ap337:5.4_0": "agamā yena patthitaṁ.",
  "tha-ap337:6.1_0": "Phusitā pañcasahassā,",
  "tha-ap337:6.2_0": "yehi pūjesahaṁ jinaṁ;",
  "tha-ap337:6.3_0": "Aḍḍhateyyasahassehi,",
  "tha-ap337:6.4_0": "devarajjaṁ akārayiṁ.",
  "tha-ap337:7.1_0": "Aḍḍhateyyasahassehi,",
  "tha-ap337:7.2_0": "cakkavattī ahosahaṁ;",
  "tha-ap337:7.3_0": "Avasesena kammena,",
  "tha-ap337:7.4_0": "arahattamapāpuṇiṁ.",
  "tha-ap337:8.1_0": "Devarājā yadā homi,",
  "tha-ap337:8.2_0": "manujādhipatī yadā;",
  "tha-ap337:8.3_0": "Tameva nāmadheyyaṁ me,",
  "tha-ap337:8.4_0": "phusito nāma homahaṁ.",
  "tha-ap337:9.1_0": "Devabhūtassa santassa,",
  "tha-ap337:9.2_0": "athāpi mānusassa vā;",
  "tha-ap337:9.3_0": "Samantā byāmato mayhaṁ,",
  "tha-ap337:9.4_0": "phusitaṁva pavassati.",
  "tha-ap337:10.1_0": "Bhavā ugghāṭitā mayhaṁ,",
  "tha-ap337:10.2_0": "kilesā jhāpitā mama;",
  "tha-ap337:10.3_0": "Sabbāsavaparikkhīṇo,",
  "tha-ap337:10.4_0": "phusitassa idaṁ phalaṁ.",
  "tha-ap337:11.1_0": "Candanasseva me kāyā,",
  "tha-ap337:11.2_0": "tathā gandho pavāyati;",
  "tha-ap337:11.3_0": "Sarīrato mama gandho,",
  "tha-ap337:11.4_0": "aḍḍhakose pavāyati.",
  "tha-ap337:12.1_0": "Dibbagandhaṁ sampavantaṁ,",
  "tha-ap337:12.2_0": "puññakammasamaṅginaṁ;",
  "tha-ap337:12.3_0": "Gandhaṁ ghatvāna jānanti,",
  "tha-ap337:12.4_0": "phusito āgato idha.",
  "tha-ap337:13.1_0": "Sākhāpalāsakaṭṭhāni,",
  "tha-ap337:13.2_0": "tiṇānipi ca sabbaso;",
  "tha-ap337:13.3_0": "Mama saṅkappamaññāya,",
  "tha-ap337:13.4_0": "gandho sampajjate khaṇe.",
  "tha-ap337:14.1_0": "Satasahassito kappe,",
  "tha-ap337:14.2_0": "candanaṁ abhipūjayiṁ;",
  "tha-ap337:14.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap337:14.4_0": "phusitassa idaṁ phalaṁ.",
  "tha-ap337:15.1_0": "Paṭisambhidā catasso,",
  "tha-ap337:15.2_0": "…pe…",
  "tha-ap337:15.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap337:16.1_0": "Itthaṁ sudaṁ āyasmā phusitakampiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap337:17.1_0": "Phusitakampiyattherassāpadānaṁ pañcamaṁ."
}