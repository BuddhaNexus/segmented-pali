{
  "vv18:0.1_0": "Vimānavatthu",
  "vv18:0.2_0": "Itthivimāna",
  "vv18:0.3_0": "Cittalatāvagga",
  "vv18:0.4_0": "1. Dāsivimānavatthu",
  "vv18:1.1_0": "“Api sakkova devindo,",
  "vv18:1.2_0": "ramme cittalatāvane;",
  "vv18:1.3_0": "Samantā anupariyāsi,",
  "vv18:1.4_0": "nārīgaṇapurakkhatā;",
  "vv18:1.5_0": "Obhāsentī disā sabbā,",
  "vv18:1.6_0": "osadhī viya tārakā.",
  "vv18:2.1_0": "Kena tetādiso vaṇṇo,",
  "vv18:2.2_0": "kena te idha mijjhati;",
  "vv18:2.3_0": "Uppajjanti ca te bhogā,",
  "vv18:2.4_0": "ye keci manaso piyā.",
  "vv18:3.1_0": "Pucchāmi taṁ devi mahānubhāve,",
  "vv18:3.2_0": "Manussabhūtā kimakāsi puññaṁ;",
  "vv18:3.3_0": "Kenāsi evaṁ jalitānubhāvā,",
  "vv18:3.4_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv18:4.1_0": "Sā devatā attamanā,",
  "vv18:4.2_0": "moggallānena pucchitā;",
  "vv18:4.3_0": "Pañhaṁ puṭṭhā viyākāsi,",
  "vv18:4.4_0": "yassa kammassidaṁ phalaṁ.",
  "vv18:5.1_0": "“Ahaṁ manussesu manussabhūtā,",
  "vv18:5.2_0": "Dāsī ahosiṁ parapessiyā kule.",
  "vv18:6.1_0": "Upāsikā cakkhumato,",
  "vv18:6.2_0": "gotamassa yasassino;",
  "vv18:6.3_0": "Tassā me nikkamo āsi,",
  "vv18:6.4_0": "sāsane tassa tādino.",
  "vv18:7.1_0": "Kāmaṁ bhijjatuyaṁ kāyo,",
  "vv18:7.2_0": "neva atthettha saṇṭhanaṁ;",
  "vv18:7.3_0": "Sikkhāpadānaṁ pañcannaṁ,",
  "vv18:7.4_0": "maggo sovatthiko sivo.",
  "vv18:8.1_0": "Akaṇṭako agahano,",
  "vv18:8.2_0": "uju sabbhi pavedito;",
  "vv18:8.3_0": "Nikkamassa phalaṁ passa,",
  "vv18:8.4_0": "yathidaṁ pāpuṇitthikā.",
  "vv18:9.1_0": "Āmantanikā raññomhi,",
  "vv18:9.2_0": "sakkassa vasavattino;",
  "vv18:9.3_0": "Saṭṭhi tūriyasahassāni,",
  "vv18:9.4_0": "paṭibodhaṁ karonti me.",
  "vv18:10.1_0": "Ālambo gaggaro bhīmo,",
  "vv18:10.2_0": "sādhuvādī ca saṁsayo;",
  "vv18:10.3_0": "Pokkharo ca suphasso ca,",
  "vv18:10.4_0": "viṇāmokkhā ca nāriyo.",
  "vv18:11.1_0": "Nandā ceva sunandā ca,",
  "vv18:11.2_0": "soṇadinnā sucimhitā;",
  "vv18:11.3_0": "Alambusā missakesī ca,",
  "vv18:11.4_0": "puṇḍarīkāti dāruṇī.",
  "vv18:12.1_0": "Eṇīphassā suphassā ca,",
  "vv18:12.2_0": "subhaddā muduvādinī;",
  "vv18:12.3_0": "Etā caññā ca seyyāse,",
  "vv18:12.4_0": "accharānaṁ pabodhikā.",
  "vv18:13.1_0": "Tā maṁ kālenupāgantvā,",
  "vv18:13.2_0": "abhibhāsanti devatā;",
  "vv18:13.3_0": "Handa naccāma gāyāma,",
  "vv18:13.4_0": "handa taṁ ramayāmase.",
  "vv18:14.1_0": "Nayidaṁ akatapuññānaṁ,",
  "vv18:14.2_0": "katapuññānamevidaṁ;",
  "vv18:14.3_0": "Asokaṁ nandanaṁ rammaṁ,",
  "vv18:14.4_0": "tidasānaṁ mahāvanaṁ.",
  "vv18:15.1_0": "Sukhaṁ akatapuññānaṁ,",
  "vv18:15.2_0": "idha natthi parattha ca;",
  "vv18:15.3_0": "Sukhañca katapuññānaṁ,",
  "vv18:15.4_0": "idha ceva parattha ca.",
  "vv18:16.1_0": "Tesaṁ sahabyakāmānaṁ,",
  "vv18:16.2_0": "kattabbaṁ kusalaṁ bahuṁ;",
  "vv18:16.3_0": "Katapuññā hi modanti,",
  "vv18:16.4_0": "sagge bhogasamaṅgino”ti.",
  "vv18:17.1_0": "Dāsivimānaṁ paṭhamaṁ."
}