{
  "ja252:0.1_0": "Jātaka",
  "ja252:0.2_0": "Tikanipāta",
  "ja252:0.3_0": "Saṅkappavagga",
  "ja252:0.4_0": "2. Tilamuṭṭhijātaka",
  "ja252:1.1_0": "“Ajjāpi me taṁ manasi,",
  "ja252:1.2_0": "yaṁ maṁ tvaṁ tilamuṭṭhiyā;",
  "ja252:1.3_0": "Bāhāya maṁ gahetvāna,",
  "ja252:1.4_0": "laṭṭhiyā anutāḷayi.",
  "ja252:2.1_0": "Nanu jīvite na ramasi,",
  "ja252:2.2_0": "yenāsi brāhmaṇāgato;",
  "ja252:2.3_0": "Yaṁ maṁ bāhā gahetvāna,",
  "ja252:2.4_0": "tikkhattuṁ anutāḷayi”.",
  "ja252:3.1_0": "“Ariyo anariyaṁ kubbantaṁ,",
  "ja252:3.2_0": "Yo daṇḍena nisedhati;",
  "ja252:3.3_0": "Sāsanaṁ taṁ na taṁ veraṁ,",
  "ja252:3.4_0": "Iti naṁ paṇḍitā vidū”ti.",
  "ja252:4.1_0": "Tilamuṭṭhijātakaṁ dutiyaṁ."
}