{
  "thag2.6:0.1_0": "Theragāthā",
  "thag2.6:0.2_0": "Dukanipāta",
  "thag2.6:0.3_0": "Paṭhamavagga",
  "thag2.6:0.4_0": "2.6 Meḷajinattheragāthā",
  "thag2.6:1.1_0": "“Yadāhaṁ dhammamassosiṁ,",
  "thag2.6:1.2_0": "bhāsamānassa satthuno;",
  "thag2.6:1.3_0": "Na kaṅkhamabhijānāmi,",
  "thag2.6:1.4_0": "sabbaññūaparājite.",
  "thag2.6:2.1_0": "Satthavāhe mahāvīre,",
  "thag2.6:2.2_0": "sārathīnaṁ varuttame;",
  "thag2.6:2.3_0": "Magge paṭipadāyaṁ vā,",
  "thag2.6:2.4_0": "kaṅkhā mayhaṁ na vijjatī”ti.",
  "thag2.6:2.5_0": "… Meḷajino thero …."
}