{
  "cp21:0.1_0": "Cariyāpiṭaka",
  "cp21:0.2_0": "Yudhañjayavagga",
  "cp21:0.3_0": "Nekkhammapāramī 1",
  "cp21:0.4_0": "Yudhañjayacariya",
  "cp21:1.1_0": "“Yadāhaṁ amitayaso,",
  "cp21:1.2_0": "Rājaputto yudhañjayo;",
  "cp21:1.3_0": "Ussāvabinduṁ sūriyātape,",
  "cp21:1.4_0": "Patitaṁ disvāna saṁvijiṁ.",
  "cp21:2.1_0": "Taññevādhipatiṁ katvā,",
  "cp21:2.2_0": "saṁvegamanubrūhayiṁ;",
  "cp21:2.3_0": "Mātāpitū ca vanditvā,",
  "cp21:2.4_0": "pabbajjamanuyācahaṁ.",
  "cp21:3.1_0": "Yācanti maṁ pañjalikā,",
  "cp21:3.2_0": "sanegamā saraṭṭhakā;",
  "cp21:3.3_0": "‘Ajjeva putta paṭipajja,",
  "cp21:3.4_0": "iddhaṁ phītaṁ mahāmahiṁ’.",
  "cp21:4.1_0": "Sarājake sahorodhe,",
  "cp21:4.2_0": "sanegame saraṭṭhake;",
  "cp21:4.3_0": "Karuṇaṁ paridevante,",
  "cp21:4.4_0": "anapekkhova pariccajiṁ.",
  "cp21:5.1_0": "Kevalaṁ pathaviṁ rajjaṁ,",
  "cp21:5.2_0": "ñātiparijanaṁ yasaṁ;",
  "cp21:5.3_0": "Cajamāno na cintesiṁ,",
  "cp21:5.4_0": "bodhiyāyeva kāraṇā.",
  "cp21:6.1_0": "Mātāpitā na me dessā,",
  "cp21:6.2_0": "napi me dessaṁ mahāyasaṁ;",
  "cp21:6.3_0": "Sabbaññutaṁ piyaṁ mayhaṁ,",
  "cp21:6.4_0": "tasmā rajjaṁ pariccajin”ti.",
  "cp21:7.1_0": "Yudhañjayacariyaṁ paṭhamaṁ."
}