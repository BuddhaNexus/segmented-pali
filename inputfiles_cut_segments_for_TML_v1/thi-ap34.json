{
  "thi-ap34:0.1_0": "Therīapadāna",
  "thi-ap34:0.2_0": "Khattiyāvagga",
  "thi-ap34:0.3_0": "4 Siṅgālamātutherīapadāna",
  "thi-ap34:1.1_0": "“Padumuttaro nāma jino,",
  "thi-ap34:1.2_0": "sabbadhammāna pāragū;",
  "thi-ap34:1.3_0": "Ito satasahassamhi,",
  "thi-ap34:1.4_0": "kappe uppajji nāyako.",
  "thi-ap34:2.1_0": "Tadāhaṁ haṁsavatiyaṁ,",
  "thi-ap34:2.2_0": "jātāmaccakule ahuṁ;",
  "thi-ap34:2.3_0": "Nānāratanapajjote,",
  "thi-ap34:2.4_0": "iddhe phīte mahaddhane.",
  "thi-ap34:3.1_0": "Pitunā saha gantvāna,",
  "thi-ap34:3.2_0": "mahājanapurakkhatā;",
  "thi-ap34:3.3_0": "Dhammaṁ buddhassa sutvāna,",
  "thi-ap34:3.4_0": "pabbajiṁ anagāriyaṁ.",
  "thi-ap34:4.1_0": "Pabbajitvāna kāyena,",
  "thi-ap34:4.2_0": "pāpakammaṁ vivajjayiṁ;",
  "thi-ap34:4.3_0": "Vacīduccaritaṁ hitvā,",
  "thi-ap34:4.4_0": "ājīvaṁ parisodhayiṁ.",
  "thi-ap34:5.1_0": "Buddhe pasannā dhamme ca,",
  "thi-ap34:5.2_0": "saṅghe ca tibbagāravā;",
  "thi-ap34:5.3_0": "Saddhammassavane yuttā,",
  "thi-ap34:5.4_0": "buddhadassanalālasā.",
  "thi-ap34:6.1_0": "Aggaṁ saddhādhimuttānaṁ,",
  "thi-ap34:6.2_0": "assosiṁ bhikkhuniṁ tadā;",
  "thi-ap34:6.3_0": "Taṁ ṭhānaṁ patthayitvāna,",
  "thi-ap34:6.4_0": "tisso sikkhā apūrayiṁ.",
  "thi-ap34:7.1_0": "Tato maṁ sugato āha,",
  "thi-ap34:7.2_0": "karuṇānugatāsayo;",
  "thi-ap34:7.3_0": "‘Yassa saddhā tathāgate,",
  "thi-ap34:7.4_0": "acalā suppatiṭṭhitā;",
  "thi-ap34:7.5_0": "Sīlañca yassa kalyāṇaṁ,",
  "thi-ap34:7.6_0": "ariyakantaṁ pasaṁsitaṁ.",
  "thi-ap34:8.1_0": "Saṅghe pasādo yassatthi,",
  "thi-ap34:8.2_0": "ujubhūtañca dassanaṁ;",
  "thi-ap34:8.3_0": "Adaliddoti taṁ āhu,",
  "thi-ap34:8.4_0": "amoghaṁ tassa jīvitaṁ.",
  "thi-ap34:9.1_0": "Tasmā saddhañca sīlañca,",
  "thi-ap34:9.2_0": "pasādaṁ dhammadassanaṁ;",
  "thi-ap34:9.3_0": "Anuyuñjetha medhāvī,",
  "thi-ap34:9.4_0": "saraṁ buddhāna sāsanaṁ’.",
  "thi-ap34:10.1_0": "Taṁ sutvāhaṁ pamuditā,",
  "thi-ap34:10.2_0": "apucchiṁ paṇidhiṁ mama;",
  "thi-ap34:10.3_0": "Tadā anomo amito,",
  "thi-ap34:10.4_0": "byākarittha vināyako;",
  "thi-ap34:10.5_0": "‘Buddhe pasannā kalyāṇī,",
  "thi-ap34:10.6_0": "lacchase taṁ supatthitaṁ.",
  "thi-ap34:11.1_0": "Satasahassito kappe,",
  "thi-ap34:11.2_0": "okkākakulasambhavo;",
  "thi-ap34:11.3_0": "Gotamo nāma gottena,",
  "thi-ap34:11.4_0": "satthā loke bhavissati.",
  "thi-ap34:12.1_0": "Tassa dhammesu dāyādā,",
  "thi-ap34:12.2_0": "orasā dhammanimmitā;",
  "thi-ap34:12.3_0": "Siṅgālakassa mātāti,",
  "thi-ap34:12.4_0": "hessati satthu sāvikā’.",
  "thi-ap34:13.1_0": "Taṁ sutvā muditā hutvā,",
  "thi-ap34:13.2_0": "yāvajīvaṁ tadā jinaṁ;",
  "thi-ap34:13.3_0": "Mettacittā paricariṁ,",
  "thi-ap34:13.4_0": "paṭipattīhi nāyakaṁ.",
  "thi-ap34:14.1_0": "Tena kammena sukatena,",
  "thi-ap34:14.2_0": "cetanāpaṇidhīhi ca;",
  "thi-ap34:14.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "thi-ap34:14.4_0": "tāvatiṁsamagacchahaṁ.",
  "thi-ap34:15.1_0": "Pacchime ca bhave dāni,",
  "thi-ap34:15.2_0": "giribbajapuruttame;",
  "thi-ap34:15.3_0": "Jātā seṭṭhikule phīte,",
  "thi-ap34:15.4_0": "mahāratanasañcaye.",
  "thi-ap34:16.1_0": "Putto siṅgālako nāma,",
  "thi-ap34:16.2_0": "mamāsi vipathe rato;",
  "thi-ap34:16.3_0": "Diṭṭhigahanapakkhando,",
  "thi-ap34:16.4_0": "disāpūjanatapparo.",
  "thi-ap34:17.1_0": "Nānādisā namassantaṁ,",
  "thi-ap34:17.2_0": "piṇḍāya nagaraṁ vajaṁ;",
  "thi-ap34:17.3_0": "Taṁ disvā ovadī buddho,",
  "thi-ap34:17.4_0": "magge ṭhatvā vināyako.",
  "thi-ap34:18.1_0": "Tassa desayato dhammaṁ,",
  "thi-ap34:18.2_0": "panādo vimhayo ahu;",
  "thi-ap34:18.3_0": "Dvekoṭinaranārīnaṁ,",
  "thi-ap34:18.4_0": "dhammābhisamayo ahu.",
  "thi-ap34:19.1_0": "Tadāhaṁ parisaṁ gantvā,",
  "thi-ap34:19.2_0": "sutvā sugatabhāsitaṁ;",
  "thi-ap34:19.3_0": "Sotāpattiphalaṁ pattā,",
  "thi-ap34:19.4_0": "pabbajiṁ anagāriyaṁ.",
  "thi-ap34:20.1_0": "Na cireneva kālena,",
  "thi-ap34:20.2_0": "buddhadassanalālasā;",
  "thi-ap34:20.3_0": "Anussatiṁ taṁ bhāvetvā,",
  "thi-ap34:20.4_0": "arahattamapāpuṇiṁ.",
  "thi-ap34:21.1_0": "Dassanatthāya buddhassa,",
  "thi-ap34:21.2_0": "sabbadā ca vajāmahaṁ;",
  "thi-ap34:21.3_0": "Atittāyeva passāmi,",
  "thi-ap34:21.4_0": "rūpaṁ nayananandanaṁ.",
  "thi-ap34:22.1_0": "Sabbapāramisambhūtaṁ,",
  "thi-ap34:22.2_0": "lakkhīnilayanaṁ varaṁ;",
  "thi-ap34:22.3_0": "Rūpaṁ sabbasubhākiṇṇaṁ,",
  "thi-ap34:22.4_0": "atittā viharāmahaṁ.",
  "thi-ap34:23.1_0": "Jino tasmiṁ guṇe tuṭṭho,",
  "thi-ap34:23.2_0": "etadagge ṭhapesi maṁ;",
  "thi-ap34:23.3_0": "‘Siṅgālakassa yā mātā,",
  "thi-ap34:23.4_0": "aggā saddhādhimuttikā’.",
  "thi-ap34:24.1_0": "Iddhīsu ca vasī homi,",
  "thi-ap34:24.2_0": "dibbāya sotadhātuyā;",
  "thi-ap34:24.3_0": "Cetopariyañāṇassa,",
  "thi-ap34:24.4_0": "vasī homi mahāmuni.",
  "thi-ap34:25.1_0": "Pubbenivāsaṁ jānāmi,",
  "thi-ap34:25.2_0": "dibbacakkhu visodhitaṁ;",
  "thi-ap34:25.3_0": "Sabbāsavā parikkhīṇā,",
  "thi-ap34:25.4_0": "natthi dāni punabbhavo.",
  "thi-ap34:26.1_0": "Atthadhammaniruttīsu,",
  "thi-ap34:26.2_0": "paṭibhāne tatheva ca;",
  "thi-ap34:26.3_0": "Ñāṇaṁ mama mahāvīra,",
  "thi-ap34:26.4_0": "uppannaṁ tava santike.",
  "thi-ap34:27.1_0": "Kilesā jhāpitā mayhaṁ,",
  "thi-ap34:27.2_0": "…pe…",
  "thi-ap34:27.3_0": "viharāmi anāsavā.",
  "thi-ap34:28.1_0": "Svāgataṁ vata me āsi,",
  "thi-ap34:28.2_0": "…pe…",
  "thi-ap34:28.3_0": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap34:29.1_0": "Paṭisambhidā catasso,",
  "thi-ap34:29.2_0": "…pe…",
  "thi-ap34:29.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "thi-ap34:30.1_0": "Itthaṁ sudaṁ siṅgālamātā bhikkhunī imā gāthāyo abhāsitthāti.",
  "thi-ap34:31.1_0": "Siṅgālamātutheriyāpadānaṁ catutthaṁ."
}