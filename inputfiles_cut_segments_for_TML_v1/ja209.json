{
  "ja209:0.1_0": "Jātaka",
  "ja209:0.2_0": "Dukanipāta",
  "ja209:0.3_0": "Nataṁdaḷhavagga",
  "ja209:0.4_0": "9. Kukkuṭajātaka",
  "ja209:1.1_0": "“Diṭṭhā mayā vane rukkhā,",
  "ja209:1.2_0": "assakaṇṇā vibhīṭakā;",
  "ja209:1.3_0": "Na tāni evaṁ sakkanti,",
  "ja209:1.4_0": "yathā tvaṁ rukkha sakkasi”.",
  "ja209:2.1_0": "“Purāṇakukkuṭo ayaṁ,",
  "ja209:2.2_0": "bhetvā pañjaramāgato;",
  "ja209:2.3_0": "Kusalo vāḷapāsānaṁ,",
  "ja209:2.4_0": "apakkamati bhāsatī”ti.",
  "ja209:3.1_0": "Kukkuṭajātakaṁ navamaṁ."
}