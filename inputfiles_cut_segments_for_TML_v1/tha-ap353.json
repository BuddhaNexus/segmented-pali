{
  "tha-ap353:0.1_0": "Therāpadāna",
  "tha-ap353:0.2_0": "Saddasaññakavagga",
  "tha-ap353:0.3_0": "1. Saddasaññakattheraapadāna",
  "tha-ap353:1.1_0": "“Migaluddo pure āsiṁ,",
  "tha-ap353:1.2_0": "araññe kānane ahaṁ;",
  "tha-ap353:1.3_0": "Tatthaddasāsiṁ sambuddhaṁ,",
  "tha-ap353:1.4_0": "devasaṅghapurakkhataṁ.",
  "tha-ap353:2.1_0": "Catusaccaṁ pakāsentaṁ,",
  "tha-ap353:2.2_0": "uddharantaṁ mahājanaṁ;",
  "tha-ap353:2.3_0": "Assosiṁ madhuraṁ vācaṁ,",
  "tha-ap353:2.4_0": "karavīkarudopamaṁ.",
  "tha-ap353:3.1_0": "Brahmasarassa munino,",
  "tha-ap353:3.2_0": "sikhino lokabandhuno;",
  "tha-ap353:3.3_0": "Ghose cittaṁ pasādetvā,",
  "tha-ap353:3.4_0": "pattomhi āsavakkhayaṁ.",
  "tha-ap353:4.1_0": "Ekattiṁse ito kappe,",
  "tha-ap353:4.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap353:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap353:4.4_0": "pasādassa idaṁ phalaṁ.",
  "tha-ap353:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap353:5.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap353:5.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap353:5.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap353:6.1_0": "Itthaṁ sudaṁ āyasmā saddasaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap353:7.1_0": "Saddasaññakattherassāpadānaṁ paṭhamaṁ."
}