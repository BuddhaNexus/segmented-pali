{
  "ja188:0.1_0": "Jātaka",
  "ja188:0.2_0": "Dukanipāta",
  "ja188:0.3_0": "Asadisavagga",
  "ja188:0.4_0": "8. Sīhakotthujātaka",
  "ja188:1.1_0": "“Sīhaṅgulī sīhanakho,",
  "ja188:1.2_0": "sīhapādapatiṭṭhito;",
  "ja188:1.3_0": "So sīho sīhasaṅghamhi,",
  "ja188:1.4_0": "eko nadati aññathā”.",
  "ja188:2.1_0": "“Mā tvaṁ nadi rājaputta,",
  "ja188:2.2_0": "appasaddo vane vasa;",
  "ja188:2.3_0": "Sarena kho taṁ jāneyyuṁ,",
  "ja188:2.4_0": "na hi te pettiko saro”ti.",
  "ja188:3.1_0": "Sīhakotthujātakaṁ aṭṭhamaṁ."
}