{
  "thig1.9:0.1_0": "Therīgāthā",
  "thig1.9:0.2_0": "Ekakanipāta",
  "thig1.9:0.3_0": "9. Bhadrātherīgāthā",
  "thig1.9:1.1_0": "“Saddhāya pabbajitvāna,",
  "thig1.9:1.2_0": "bhadre bhadraratā bhava;",
  "thig1.9:1.3_0": "Bhāvehi kusale dhamme,",
  "thig1.9:1.4_0": "yogakkhemamanuttaran”ti.",
  "thig1.9:2.1_0": "… Bhadrā therī …."
}