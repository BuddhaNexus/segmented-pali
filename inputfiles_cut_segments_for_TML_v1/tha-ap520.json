{
  "tha-ap520:0.1_0": "Therāpadāna",
  "tha-ap520:0.2_0": "Phaladāyakavagga",
  "tha-ap520:0.3_0": "8. Ghosasaññakattheraapadāna",
  "tha-ap520:1.1_0": "“Migaluddo pure āsiṁ,",
  "tha-ap520:1.2_0": "araññe vipine ahaṁ;",
  "tha-ap520:1.3_0": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap520:1.4_0": "devasaṅghapurakkhataṁ.",
  "tha-ap520:2.1_0": "Catusaccaṁ pakāsentaṁ,",
  "tha-ap520:2.2_0": "desentaṁ amataṁ padaṁ;",
  "tha-ap520:2.3_0": "Assosiṁ madhuraṁ dhammaṁ,",
  "tha-ap520:2.4_0": "sikhino lokabandhuno.",
  "tha-ap520:3.1_0": "Ghose cittaṁ pasādesiṁ,",
  "tha-ap520:3.2_0": "asamappaṭipuggale;",
  "tha-ap520:3.3_0": "Tattha cittaṁ pasādetvā,",
  "tha-ap520:3.4_0": "uttariṁ duttaraṁ bhavaṁ.",
  "tha-ap520:4.1_0": "Ekatiṁse ito kappe,",
  "tha-ap520:4.2_0": "yaṁ saññamalabhiṁ tadā;",
  "tha-ap520:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap520:4.4_0": "ghosasaññāyidaṁ phalaṁ.",
  "tha-ap520:5.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap520:5.2_0": "…pe…",
  "tha-ap520:5.3_0": "viharāmi anāsavo.",
  "tha-ap520:6.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap520:6.2_0": "…pe…",
  "tha-ap520:6.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap520:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap520:7.2_0": "…pe…",
  "tha-ap520:7.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap520:8.1_0": "Itthaṁ sudaṁ āyasmā ghosasaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap520:9.1_0": "Ghosasaññakattherassāpadānaṁ aṭṭhamaṁ."
}