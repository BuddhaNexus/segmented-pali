{
  "snp5.8:0.1_0": "Sutta Nipāta 5.8",
  "snp5.8:0.2_0": "Nandamāṇavapucchā",
  "snp5.8:1.1_0": "“Santi loke munayo,",
  "snp5.8:1.2_0": "(iccāyasmā nando)",
  "snp5.8:1.3_0": "Janā vadanti tayidaṁ kathaṁsu;",
  "snp5.8:1.4_0": "Ñāṇūpapannaṁ no muniṁ vadanti,",
  "snp5.8:1.5_0": "Udāhu ve jīvitenūpapannaṁ”.",
  "snp5.8:2.1_0": "“Na diṭṭhiyā na sutiyā na ñāṇena, (…)",
  "snp5.8:2.2_0": "Munīdha nanda kusalā vadanti;",
  "snp5.8:2.3_0": "Visenikatvā anīghā nirāsā,",
  "snp5.8:2.4_0": "Caranti ye te munayoti brūmi”.",
  "snp5.8:3.1_0": "“Ye kecime samaṇabrāhmaṇāse,",
  "snp5.8:3.2_0": "(iccāyasmā nando)",
  "snp5.8:3.3_0": "Diṭṭhassutenāpi vadanti suddhiṁ;",
  "snp5.8:3.4_0": "Sīlabbatenāpi vadanti suddhiṁ,",
  "snp5.8:3.5_0": "Anekarūpena vadanti suddhiṁ.",
  "snp5.8:3.6_0": "Kaccissu te bhagavā tattha yatā carantā,",
  "snp5.8:3.7_0": "Atāru jātiñca jarañca mārisa;",
  "snp5.8:3.8_0": "Pucchāmi taṁ bhagavā brūhi me taṁ”.",
  "snp5.8:4.1_0": "“Ye kecime samaṇabrāhmaṇāse,",
  "snp5.8:4.2_0": "(nandāti bhagavā)",
  "snp5.8:4.3_0": "Diṭṭhassutenāpi vadanti suddhiṁ;",
  "snp5.8:4.4_0": "Sīlabbatenāpi vadanti suddhiṁ,",
  "snp5.8:4.5_0": "Anekarūpena vadanti suddhiṁ;",
  "snp5.8:4.6_0": "Kiñcāpi te tattha yatā caranti,",
  "snp5.8:4.7_0": "Nātariṁsu jātijaranti brūmi”.",
  "snp5.8:5.1_0": "“Ye kecime samaṇabrāhmaṇāse,",
  "snp5.8:5.2_0": "(iccāyasmā nando)",
  "snp5.8:5.3_0": "Diṭṭhassutenāpi vadanti suddhiṁ;",
  "snp5.8:5.4_0": "Sīlabbatenāpi vadanti suddhiṁ,",
  "snp5.8:5.5_0": "Anekarūpena vadanti suddhiṁ.",
  "snp5.8:5.6_0": "Te ce muni brūsi anoghatiṇṇe,",
  "snp5.8:5.7_0": "Atha ko carahi devamanussaloke;",
  "snp5.8:5.8_0": "Atāri jātiñca jarañca mārisa,",
  "snp5.8:5.9_0": "Pucchāmi taṁ bhagavā brūhi me taṁ”.",
  "snp5.8:6.1_0": "“Nāhaṁ sabbe samaṇabrāhmaṇāse,",
  "snp5.8:6.2_0": "(nandāti bhagavā)",
  "snp5.8:6.3_0": "Jātijarāya nivutāti brūmi;",
  "snp5.8:6.4_0": "Ye sīdha diṭṭhaṁ va sutaṁ mutaṁ vā,",
  "snp5.8:6.5_0": "Sīlabbataṁ vāpi pahāya sabbaṁ.",
  "snp5.8:6.6_0": "Anekarūpampi pahāya sabbaṁ,",
  "snp5.8:6.7_0": "Taṇhaṁ pariññāya anāsavāse;",
  "snp5.8:6.8_0": "Te ve narā oghatiṇṇāti brūmi”.",
  "snp5.8:7.1_0": "“Etābhinandāmi vaco mahesino,",
  "snp5.8:7.2_0": "Sukittitaṁ gotamanūpadhīkaṁ;",
  "snp5.8:7.3_0": "Ye sīdha diṭṭhaṁ va sutaṁ mutaṁ vā,",
  "snp5.8:7.4_0": "Sīlabbataṁ vāpi pahāya sabbaṁ.",
  "snp5.8:7.5_0": "Anekarūpampi pahāya sabbaṁ,",
  "snp5.8:7.6_0": "Taṇhaṁ pariññāya anāsavāse;",
  "snp5.8:7.7_0": "Ahampi te oghatiṇṇāti brūmī”ti.",
  "snp5.8:7.8_0": "Nandamāṇavapucchā sattamā."
}