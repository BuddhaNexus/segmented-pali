{
  "tha-ap250:0.1_0": "Therāpadāna",
  "tha-ap250:0.2_0": "Tuvaradāyakavagga",
  "tha-ap250:0.3_0": "8 Sattalipupphapūjakattheraapadāna",
  "tha-ap250:1.1_0": "“Satta sattalipupphāni,",
  "tha-ap250:1.2_0": "sīse katvānahaṁ tadā;",
  "tha-ap250:1.3_0": "Buddhassa abhiropesiṁ,",
  "tha-ap250:1.4_0": "vessabhumhi naruttame.",
  "tha-ap250:2.1_0": "Ekattiṁse ito kappe,",
  "tha-ap250:2.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap250:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap250:2.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap250:3.1_0": "Paṭisambhidā catasso,",
  "tha-ap250:3.2_0": "…pe…",
  "tha-ap250:3.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap250:4.1_0": "Itthaṁ sudaṁ āyasmā sattalipupphapūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap250:5.1_0": "Sattalipupphapūjakattherassāpadānaṁ aṭṭhamaṁ."
}