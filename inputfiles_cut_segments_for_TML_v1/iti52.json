{
  "iti52:0.1_0": "Itivuttaka 52",
  "iti52:0.2_0": "Tikanipāta",
  "iti52:0.3_0": "Paṭhamavagga",
  "iti52:0.4_0": "Paṭhamavedanāsutta",
  "iti52:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti52:2.1_0": "“Tisso imā, bhikkhave, vedanā.",
  "iti52:2.2_0": "Katamā tisso?",
  "iti52:2.3_0": "Sukhā vedanā, dukkhā vedanā, adukkhamasukhā vedanā—",
  "iti52:2.4_0": "imā kho, bhikkhave, tisso vedanā”ti.",
  "iti52:2.5_0": "Etamatthaṁ bhagavā avoca.",
  "iti52:2.6_0": "Tatthetaṁ iti vuccati:",
  "iti52:3.1_0": "“Samāhito sampajāno,",
  "iti52:3.2_0": "sato buddhassa sāvako;",
  "iti52:3.3_0": "Vedanā ca pajānāti,",
  "iti52:3.4_0": "vedanānañca sambhavaṁ.",
  "iti52:4.1_0": "Yattha cetā nirujjhanti,",
  "iti52:4.2_0": "maggañca khayagāminaṁ;",
  "iti52:4.3_0": "Vedanānaṁ khayā bhikkhu,",
  "iti52:4.4_0": "nicchāto parinibbuto”ti.",
  "iti52:5.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti52:5.2_0": "Tatiyaṁ."
}