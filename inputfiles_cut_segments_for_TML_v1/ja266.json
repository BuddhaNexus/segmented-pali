{
  "ja266:0.1_0": "Jātaka",
  "ja266:0.2_0": "Tikanipāta",
  "ja266:0.3_0": "Padumavagga",
  "ja266:0.4_0": "6. Vātaggasindhavajātaka",
  "ja266:1.1_0": "“Yenāsi kisiyā paṇḍu,",
  "ja266:1.2_0": "yena bhattaṁ na ruccati;",
  "ja266:1.3_0": "Ayaṁ so āgato bhattā,",
  "ja266:1.4_0": "kasmā dāni palāyasi”.",
  "ja266:2.1_0": "“Sace panādikeneva,",
  "ja266:2.2_0": "santhavo nāma jāyati;",
  "ja266:2.3_0": "Yaso hāyati itthīnaṁ,",
  "ja266:2.4_0": "tasmā tāta palāyahaṁ”.",
  "ja266:3.1_0": "Yasassinaṁ kule jātaṁ,",
  "ja266:3.2_0": "āgataṁ yā na icchati;",
  "ja266:3.3_0": "Socati cirarattāya,",
  "ja266:3.4_0": "vātaggamiva bhaddalīti.",
  "ja266:4.1_0": "Vātaggasindhavajātakaṁ chaṭṭhaṁ."
}