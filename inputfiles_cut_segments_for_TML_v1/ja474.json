{
  "ja474:0.1_0": "Jātaka",
  "ja474:0.2_0": "Terasakanipāta",
  "ja474:0.3_0": "Ambavagga",
  "ja474:0.4_0": "1. Ambajātaka",
  "ja474:1.1_0": "“Ahāsi me ambaphalāni pubbe,",
  "ja474:1.2_0": "Aṇūni thūlāni ca brahmacāri;",
  "ja474:1.3_0": "Teheva mantehi na dāni tuyhaṁ,",
  "ja474:1.4_0": "Dumapphalā pātubhavanti brahme”.",
  "ja474:2.1_0": "“Nakkhattayogaṁ paṭimānayāmi,",
  "ja474:2.2_0": "Khaṇaṁ muhuttañca mante na passaṁ;",
  "ja474:2.3_0": "Nakkhattayogañca khaṇañca laddhā,",
  "ja474:2.4_0": "Addhāharissambaphalaṁ pahūtaṁ”.",
  "ja474:3.1_0": "“Nakkhattayogaṁ na pure abhāṇi,",
  "ja474:3.2_0": "Khaṇaṁ muhuttaṁ na pure asaṁsi;",
  "ja474:3.3_0": "Sayaṁ harī ambaphalaṁ pahūtaṁ,",
  "ja474:3.4_0": "Vaṇṇena gandhena rasenupetaṁ.",
  "ja474:4.1_0": "Mantābhijappena pure hi tuyhaṁ,",
  "ja474:4.2_0": "Dumapphalā pātubhavanti brahme;",
  "ja474:4.3_0": "Svājja na pāresi jappampi mantaṁ,",
  "ja474:4.4_0": "Ayaṁ so ko nāma tavajja dhammo”.",
  "ja474:5.1_0": "“Caṇḍālaputto mama sampadāsi,",
  "ja474:5.2_0": "Dhammena mante pakatiñca saṁsi;",
  "ja474:5.3_0": "Mā cassu me pucchito nāmagottaṁ,",
  "ja474:5.4_0": "Guyhittho atthaṁ vijaheyya manto.",
  "ja474:6.1_0": "Sohaṁ janindena janamhi puṭṭho,",
  "ja474:6.2_0": "Makkhābhibhūto alikaṁ abhāṇiṁ;",
  "ja474:6.3_0": "‘Mantā ime brāhmaṇassā’ti micchā,",
  "ja474:6.4_0": "Pahīnamanto kapaṇo rudāmi”.",
  "ja474:7.1_0": "“Eraṇḍā pucimandā vā,",
  "ja474:7.2_0": "atha vā pālibhaddakā;",
  "ja474:7.3_0": "Madhuṁ madhutthiko vinde,",
  "ja474:7.4_0": "so hi tassa dumuttamo.",
  "ja474:8.1_0": "Khattiyā brāhmaṇā vessā,",
  "ja474:8.2_0": "suddā caṇḍālapukkusā;",
  "ja474:8.3_0": "Yamhā dhammaṁ vijāneyya,",
  "ja474:8.4_0": "so hi tassa naruttamo.",
  "ja474:9.1_0": "Imassa daṇḍañca vadhañca datvā,",
  "ja474:9.2_0": "Gale gahetvā khalayātha jammaṁ;",
  "ja474:9.3_0": "Yo uttamatthaṁ kasirena laddhaṁ,",
  "ja474:9.4_0": "Mānātimānena vināsayittha”.",
  "ja474:10.1_0": "“Yathā samaṁ maññamāno pateyya,",
  "ja474:10.2_0": "Sobbhaṁ guhaṁ narakaṁ pūtipādaṁ;",
  "ja474:10.3_0": "Rajjūti vā akkame kaṇhasappaṁ,",
  "ja474:10.4_0": "Andho yathā jotimadhiṭṭhaheyya;",
  "ja474:10.5_0": "Evampi maṁ tvaṁ khalitaṁ sapañña,",
  "ja474:10.6_0": "Pahīnamantassa punappadāhi”.",
  "ja474:11.1_0": "“Dhammena mantaṁ tava sampadāsiṁ,",
  "ja474:11.2_0": "Tuvampi dhammena pariggahesi;",
  "ja474:11.3_0": "Pakatimpi te attamano asaṁsiṁ,",
  "ja474:11.4_0": "Dhamme ṭhitaṁ taṁ na jaheyya manto.",
  "ja474:12.1_0": "Yo bāla mantaṁ kasirena laddhaṁ,",
  "ja474:12.2_0": "Yaṁ dullabhaṁ ajja manussaloke;",
  "ja474:12.3_0": "Kiñcāpi laddhā jīvituṁ appapañño,",
  "ja474:12.4_0": "Vināsayī alikaṁ bhāsamāno.",
  "ja474:13.1_0": "Bālassa mūḷhassa akataññuno ca,",
  "ja474:13.2_0": "Musā bhaṇantassa asaññatassa;",
  "ja474:13.3_0": "Mante mayaṁ tādisake na dema,",
  "ja474:13.4_0": "Kuto mantā gaccha na mayha ruccasī”ti.",
  "ja474:14.1_0": "Ambajātakaṁ paṭhamaṁ."
}