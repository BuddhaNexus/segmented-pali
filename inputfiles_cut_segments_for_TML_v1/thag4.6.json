{
  "thag4.6:0.1_0": "Theragāthā",
  "thag4.6:0.2_0": "Catukkanipāta",
  "thag4.6:0.3_0": "Paṭhamavagga",
  "thag4.6:0.4_0": "4.6. Senakattheragāthā",
  "thag4.6:1.1_0": "“Svāgataṁ vata me āsi,",
  "thag4.6:1.2_0": "gayāyaṁ gayaphagguyā;",
  "thag4.6:1.3_0": "Yaṁ addasāsiṁ sambuddhaṁ,",
  "thag4.6:1.4_0": "desentaṁ dhammamuttamaṁ.",
  "thag4.6:2.1_0": "Mahappabhaṁ gaṇācariyaṁ,",
  "thag4.6:2.2_0": "aggapattaṁ vināyakaṁ;",
  "thag4.6:2.3_0": "Sadevakassa lokassa,",
  "thag4.6:2.4_0": "jinaṁ atuladassanaṁ.",
  "thag4.6:3.1_0": "Mahānāgaṁ mahāvīraṁ,",
  "thag4.6:3.2_0": "mahājutimanāsavaṁ;",
  "thag4.6:3.3_0": "Sabbāsavaparikkhīṇaṁ,",
  "thag4.6:3.4_0": "satthāramakutobhayaṁ.",
  "thag4.6:4.1_0": "Cirasaṅkiliṭṭhaṁ vata maṁ,",
  "thag4.6:4.2_0": "diṭṭhisandānabandhitaṁ;",
  "thag4.6:4.3_0": "Vimocayi so bhagavā,",
  "thag4.6:4.4_0": "sabbaganthehi senakan”ti.",
  "thag4.6:4.5_0": "… Senako thero …."
}