{
  "ja26:0.1_0": "Jātaka",
  "ja26:0.2_0": "Ekakanipāta",
  "ja26:0.3_0": "Kuruṅgavagga",
  "ja26:0.4_0": "6. Mahiḷāmukhajātaka",
  "ja26:1.1_0": "“Purāṇacorāna vaco nisamma,",
  "ja26:1.2_0": "Mahiḷāmukho pothayamanvacārī;",
  "ja26:1.3_0": "Susaññatānañhi vaco nisamma,",
  "ja26:1.4_0": "Gajuttamo sabbaguṇesu aṭṭhā”ti.",
  "ja26:2.1_0": "Mahiḷāmukhajātakaṁ chaṭṭhaṁ."
}