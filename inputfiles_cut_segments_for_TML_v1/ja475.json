{
  "ja475:0.1_0": "Jātaka",
  "ja475:0.2_0": "Terasakanipāta",
  "ja475:0.3_0": "Ambavagga",
  "ja475:0.4_0": "2. Phandanajātaka",
  "ja475:1.1_0": "“Kuṭhārihattho puriso,",
  "ja475:1.2_0": "vanamogayha tiṭṭhasi;",
  "ja475:1.3_0": "Puṭṭho me samma akkhāhi,",
  "ja475:1.4_0": "kiṁ dāruṁ chetumicchasi”.",
  "ja475:2.1_0": "“Isso vanāni carasi,",
  "ja475:2.2_0": "samāni visamāni ca;",
  "ja475:2.3_0": "Puṭṭho me samma akkhāhi,",
  "ja475:2.4_0": "kiṁ dāruṁ nemiyā daḷhaṁ”.",
  "ja475:3.1_0": "“Neva sālo na khadiro,",
  "ja475:3.2_0": "nāssakaṇṇo kuto dhavo;",
  "ja475:3.3_0": "Rukkho ca phandano nāma,",
  "ja475:3.4_0": "taṁ dāruṁ nemiyā daḷhaṁ”.",
  "ja475:4.1_0": "“Kīdisānissa pattāni,",
  "ja475:4.2_0": "khandho vā pana kīdiso;",
  "ja475:4.3_0": "Puṭṭho me samma akkhāhi,",
  "ja475:4.4_0": "yathā jānemu phandanaṁ”.",
  "ja475:5.1_0": "“Yassa sākhā palambanti,",
  "ja475:5.2_0": "namanti na ca bhañjare;",
  "ja475:5.3_0": "So rukkho phandano nāma,",
  "ja475:5.4_0": "yassa mūle ahaṁ ṭhito.",
  "ja475:6.1_0": "Arānaṁ cakkanābhīnaṁ,",
  "ja475:6.2_0": "īsānemirathassa ca;",
  "ja475:6.3_0": "Sabbassa te kammaniyo,",
  "ja475:6.4_0": "ayaṁ hessati phandano”.",
  "ja475:7.1_0": "“Iti phandanarukkhopi,",
  "ja475:7.2_0": "tāvade ajjhabhāsatha;",
  "ja475:7.3_0": "Mayhampi vacanaṁ atthi,",
  "ja475:7.4_0": "bhāradvāja suṇohi me.",
  "ja475:8.1_0": "Issassa upakkhandhamhā,",
  "ja475:8.2_0": "Ukkacca caturaṅgulaṁ;",
  "ja475:8.3_0": "Tena nemiṁ pasāresi,",
  "ja475:8.4_0": "Evaṁ daḷhataraṁ siyā.",
  "ja475:9.1_0": "Iti phandanarukkhopi,",
  "ja475:9.2_0": "veraṁ appesi tāvade;",
  "ja475:9.3_0": "Jātānañca ajātānaṁ,",
  "ja475:9.4_0": "issānaṁ dukkhamāvahi.",
  "ja475:10.1_0": "Iccevaṁ phandano issaṁ,",
  "ja475:10.2_0": "isso ca pana phandanaṁ;",
  "ja475:10.3_0": "Aññamaññaṁ vivādena,",
  "ja475:10.4_0": "aññamaññamaghātayuṁ.",
  "ja475:11.1_0": "Evameva manussānaṁ,",
  "ja475:11.2_0": "vivādo yattha jāyati;",
  "ja475:11.3_0": "Mayūranaccaṁ naccanti,",
  "ja475:11.4_0": "yathā te issaphandanā.",
  "ja475:12.1_0": "Taṁ vo vadāmi bhaddaṁ vo,",
  "ja475:12.2_0": "yāvantettha samāgatā;",
  "ja475:12.3_0": "Sammodatha mā vivadatha,",
  "ja475:12.4_0": "mā hotha issaphandanā.",
  "ja475:13.1_0": "Sāmaggimeva sikkhetha,",
  "ja475:13.2_0": "buddhehetaṁ pasaṁsitaṁ;",
  "ja475:13.3_0": "Sāmaggirato dhammaṭṭho,",
  "ja475:13.4_0": "yogakkhemā na dhaṁsatī”ti.",
  "ja475:14.1_0": "Phandanajātakaṁ dutiyaṁ."
}