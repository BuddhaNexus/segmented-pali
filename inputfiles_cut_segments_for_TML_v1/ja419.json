{
  "ja419:0.1_0": "Jātaka",
  "ja419:0.2_0": "Aṭṭhakanipāta",
  "ja419:0.3_0": "Kaccānivagga",
  "ja419:0.4_0": "3. Sulasājātaka",
  "ja419:1.1_0": "“Idaṁ suvaṇṇakāyūraṁ,",
  "ja419:1.2_0": "muttā veḷuriyā bahū;",
  "ja419:1.3_0": "Sabbaṁ harassu bhaddante,",
  "ja419:1.4_0": "mañca dāsīti sāvaya”.",
  "ja419:2.1_0": "“Oropayassu kalyāṇi,",
  "ja419:2.2_0": "mā bāḷhaṁ paridevasi;",
  "ja419:2.3_0": "Na cāhaṁ abhijānāmi,",
  "ja419:2.4_0": "ahantvā dhanamābhataṁ”.",
  "ja419:3.1_0": "“Yato sarāmi attānaṁ,",
  "ja419:3.2_0": "yato pattāsmi viññutaṁ;",
  "ja419:3.3_0": "Na cāhaṁ abhijānāmi,",
  "ja419:3.4_0": "aññaṁ piyataraṁ tayā.",
  "ja419:4.1_0": "Ehi taṁ upagūhissaṁ,",
  "ja419:4.2_0": "karissañca padakkhiṇaṁ;",
  "ja419:4.3_0": "Na hi dāni puna atthi,",
  "ja419:4.4_0": "mama tuyhañca saṅgamo”.",
  "ja419:5.1_0": "“Na hi sabbesu ṭhānesu,",
  "ja419:5.2_0": "puriso hoti paṇḍito;",
  "ja419:5.3_0": "Itthīpi paṇḍitā hoti,",
  "ja419:5.4_0": "tattha tattha vicakkhaṇā.",
  "ja419:6.1_0": "Na hi sabbesu ṭhānesu,",
  "ja419:6.2_0": "puriso hoti paṇḍito;",
  "ja419:6.3_0": "Itthīpi paṇḍitā hoti,",
  "ja419:6.4_0": "lahuṁ atthaṁ vicintikā.",
  "ja419:7.1_0": "Lahuñca vata khippañca,",
  "ja419:7.2_0": "nikaṭṭhe samacetayi;",
  "ja419:7.3_0": "Migaṁ puṇṇāyateneva,",
  "ja419:7.4_0": "sulasā sattukaṁ vadhi.",
  "ja419:8.1_0": "Yodha uppatitaṁ atthaṁ,",
  "ja419:8.2_0": "na khippamanubujjhati;",
  "ja419:8.3_0": "So haññati mandamati,",
  "ja419:8.4_0": "corova girigabbhare.",
  "ja419:9.1_0": "Yo ca uppatitaṁ atthaṁ,",
  "ja419:9.2_0": "khippameva nibodhati;",
  "ja419:9.3_0": "Muccate sattusambādhā,",
  "ja419:9.4_0": "sulasā sattukāmivā”ti.",
  "ja419:10.1_0": "Sulasājātakaṁ tatiyaṁ."
}