{
  "ja225:0.1_0": "Jātaka",
  "ja225:0.2_0": "Dukanipāta",
  "ja225:0.3_0": "Kāsāvavagga",
  "ja225:0.4_0": "5. Khantivaṇṇajātaka",
  "ja225:1.1_0": "“Atthi me puriso deva,",
  "ja225:1.2_0": "sabbakiccesu byāvaṭo;",
  "ja225:1.3_0": "Tassa cekoparādhatthi,",
  "ja225:1.4_0": "tattha tvaṁ kinti maññasi”.",
  "ja225:2.1_0": "“Amhākampatthi puriso,",
  "ja225:2.2_0": "ediso idha vijjati;",
  "ja225:2.3_0": "Dullabho aṅgasampanno,",
  "ja225:2.4_0": "khantirasmāka ruccatī”ti.",
  "ja225:3.1_0": "Khantivaṇṇajātakaṁ pañcamaṁ."
}