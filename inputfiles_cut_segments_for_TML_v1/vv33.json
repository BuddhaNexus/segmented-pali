{
  "vv33:0.1_0": "Vimānavatthu",
  "vv33:0.2_0": "Itthivimāna",
  "vv33:0.3_0": "Pāricchattakavagga",
  "vv33:0.4_0": "Guttilavimānavatthu",
  "vv33:1.0.1_0": "1. Vatthuttamadāyikāvimānavatthu",
  "vv33:1.1_0": "“Sattatantiṁ sumadhuraṁ,",
  "vv33:1.2_0": "Rāmaṇeyyaṁ avācayiṁ;",
  "vv33:1.3_0": "So maṁ raṅgamhi avheti,",
  "vv33:1.4_0": "‘Saraṇaṁ me hohi kosiyā’”ti.",
  "vv33:2.1_0": "“Ahaṁ te saraṇaṁ homi,",
  "vv33:2.2_0": "Ahamācariyapūjako;",
  "vv33:2.3_0": "Na taṁ jayissati sisso,",
  "vv33:2.4_0": "Sissamācariya jessasī”ti.",
  "vv33:3.1_0": "“Abhikkantena vaṇṇena,",
  "vv33:3.2_0": "yā tvaṁ tiṭṭhasi devate;",
  "vv33:3.3_0": "Obhāsentī disā sabbā,",
  "vv33:3.4_0": "osadhī viya tārakā.",
  "vv33:4.1_0": "Kena tetādiso vaṇṇo,",
  "vv33:4.2_0": "kena te idha mijjhati;",
  "vv33:4.3_0": "Uppajjanti ca te bhogā,",
  "vv33:4.4_0": "ye keci manaso piyā.",
  "vv33:5.1_0": "Pucchāmi taṁ devi mahānubhāve,",
  "vv33:5.2_0": "Manussabhūtā kimakāsi puññaṁ;",
  "vv33:5.3_0": "Kenāsi evaṁ jalitānubhāvā,",
  "vv33:5.4_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv33:6.1_0": "Sā devatā attamanā,",
  "vv33:6.2_0": "moggallānena pucchitā;",
  "vv33:6.3_0": "Pañhaṁ puṭṭhā viyākāsi,",
  "vv33:6.4_0": "yassa kammassidaṁ phalaṁ.",
  "vv33:7.1_0": "“Vatthuttamadāyikā nārī,",
  "vv33:7.2_0": "Pavarā hoti naresu nārīsu;",
  "vv33:7.3_0": "Evaṁ piyarūpadāyikā manāpaṁ,",
  "vv33:7.4_0": "Dibbaṁ sā labhate upecca ṭhānaṁ.",
  "vv33:8.1_0": "Tassā me passa vimānaṁ,",
  "vv33:8.2_0": "Accharā kāmavaṇṇinīhamasmi;",
  "vv33:8.3_0": "Accharāsahassassāhaṁ,",
  "vv33:8.4_0": "Pavarā passa puññānaṁ vipākaṁ.",
  "vv33:9.1_0": "Tena metādiso vaṇṇo,",
  "vv33:9.2_0": "tena me idha mijjhati;",
  "vv33:9.3_0": "Uppajjanti ca me bhogā,",
  "vv33:9.4_0": "ye keci manaso piyā.",
  "vv33:10.1_0": "Akkhāmi te bhikkhu mahānubhāva,",
  "vv33:10.2_0": "Manussabhūtā yamakāsi puññaṁ;",
  "vv33:10.3_0": "Tenamhi evaṁ jalitānubhāvā,",
  "vv33:10.4_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv33:11.1_0": "(Anantaraṁ caturavimānaṁ yathā vatthadāyikāvimānaṁ tathā vitthāretabbaṁ.)",
  "vv33:12.0_0": "2. Pupphuttamadāyikāvimānavatthu",
