  "vv33:82.0_0": "18. Phārusakadāyikāvimānavatthu",
  "vv33:82.1_0": "“Phārusakaṁ ahamadāsiṁ,",
  "vv33:82.2_0": "Bhikkhuno piṇḍāya carantassa;",
  "vv33:82.3_0": "…pe…",
  "vv33:83.0_0": "19. Hatthappatāpakadāyikāvimānavatthu",
  "vv33:83.1_0": "“Hatthappatāpakaṁ ahamadāsiṁ,",
  "vv33:83.2_0": "Bhikkhuno piṇḍāya carantassa;",
  "vv33:83.3_0": "…pe…",
  "vv33:84.0_0": "20. Sākamuṭṭhidāyikāvimānavatthu",
  "vv33:84.1_0": "“Sākamuṭṭhiṁ ahamadāsiṁ,",
  "vv33:84.2_0": "Bhikkhuno panthapaṭipannassa;",
  "vv33:84.3_0": "…pe…",
  "vv33:85.0_0": "21. Pupphakamuṭṭhidāyikāvimānavatthu",
  "vv33:85.1_0": "“Pupphakamuṭṭhiṁ ahamadāsiṁ,",
  "vv33:85.2_0": "Bhikkhuno piṇḍāya carantassa;",
  "vv33:85.3_0": "…pe…",
  "vv33:86.0_0": "22. Mūlakadāyikāvimānavatthu",
  "vv33:86.1_0": "“Mūlakaṁ ahamadāsiṁ,",
  "vv33:86.2_0": "Bhikkhuno piṇḍāya carantassa;",
  "vv33:86.3_0": "…pe…",
  "vv33:87.0_0": "23. Nimbamuṭṭhidāyikāvimānavatthu",
  "vv33:87.1_0": "“Nimbamuṭṭhiṁ ahamadāsiṁ,",
  "vv33:87.2_0": "Bhikkhuno piṇḍāya carantassa;",
  "vv33:87.3_0": "…pe…",
  "vv33:88.0_0": "24. Ambakañjikadāyikāvimānavatthu",
  "vv33:88.1_0": "“Ambakañjikaṁ ahamadāsiṁ,",
  "vv33:88.2_0": "Bhikkhuno piṇḍāya carantassa;",
  "vv33:88.3_0": "…pe…",
  "vv33:89.0_0": "25. Doṇinimmajjanidāyikāvimānavatthu",
  "vv33:89.1_0": "“Doṇinimmajjaniṁ ahamadāsiṁ,",
  "vv33:89.2_0": "Bhikkhuno piṇḍāya carantassa;",
  "vv33:89.3_0": "…pe…",
  "vv33:90.0_0": "26. Kāyabandhanadāyikāvimānavatthu",
  "vv33:90.1_0": "“Kāyabandhanaṁ ahamadāsiṁ,",
  "vv33:90.2_0": "Bhikkhuno piṇḍāya carantassa;",
  "vv33:90.3_0": "…pe…",
  "vv33:91.0_0": "27. Aṁsabaddhakadāyikāvimānavatthu",
  "vv33:91.1_0": "“Aṁsabaddhakaṁ ahamadāsiṁ,",
  "vv33:91.2_0": "Bhikkhuno piṇḍāya carantassa;",
  "vv33:91.3_0": "…pe…",
  "vv33:92.0_0": "28. Āyogapaṭṭadāyikāvimānavatthu",
  "vv33:92.1_0": "“Āyogapaṭṭaṁ ahamadāsiṁ,",
  "vv33:92.2_0": "Bhikkhuno piṇḍāya carantassa;",
  "vv33:92.3_0": "…pe…",
  "vv33:93.0_0": "29. Vidhūpanadāyikāvimānavatthu",
  "vv33:93.1_0": "“Vidhūpanaṁ ahamadāsiṁ,",
  "vv33:93.2_0": "Bhikkhuno piṇḍāya carantassa;",
  "vv33:93.3_0": "…pe…",
  "vv33:94.0_0": "30. Tālavaṇṭadāyikāvimānavatthu",
  "vv33:94.1_0": "“Tālavaṇṭaṁ ahamadāsiṁ,",
  "vv33:94.2_0": "Bhikkhuno piṇḍāya carantassa;",
  "vv33:94.3_0": "…pe…",
  "vv33:95.0_0": "31. Morahatthadāyikāvimānavatthu",
  "vv33:95.1_0": "“Morahatthaṁ ahamadāsiṁ,",
  "vv33:95.2_0": "Bhikkhuno piṇḍāya carantassa;",
  "vv33:95.3_0": "…pe…",
  "vv33:96.0_0": "32. Chattadāyikāvimānavatthu",
  "vv33:96.1_0": "“Chattaṁ ahamadāsiṁ,",
  "vv33:96.2_0": "Bhikkhuno piṇḍāya carantassa;",
  "vv33:96.3_0": "…pe…",
  "vv33:97.0_0": "33. Upāhanadāyikāvimānavatthu",
  "vv33:97.1_0": "“Upāhanaṁ ahamadāsiṁ,",
  "vv33:97.2_0": "Bhikkhuno piṇḍāya carantassa;",
  "vv33:97.3_0": "…pe…",
  "vv33:98.0_0": "34. Pūvadāyikāvimānavatthu",
  "vv33:98.1_0": "“Pūvaṁ ahamadāsiṁ,",
  "vv33:98.2_0": "Bhikkhuno piṇḍāya carantassa;",
  "vv33:98.3_0": "…pe…",
  "vv33:99.0_0": "35. Modakadāyikāvimānavatthu",
  "vv33:99.1_0": "“Modakaṁ ahamadāsiṁ,",
  "vv33:99.2_0": "Bhikkhuno piṇḍāya carantassa;",
  "vv33:99.3_0": "…pe…",
  "vv33:100.0_0": "36. Sakkhalikadāyikāvimānavatthu",
  "vv33:100.1_0": "“Sakkhalikaṁ ahamadāsiṁ,",
  "vv33:100.2_0": "Bhikkhuno piṇḍāya carantassa;",
  "vv33:100.3_0": "…pe…",
  "vv33:101.1_0": "Tassā me passa vimānaṁ,",
  "vv33:101.2_0": "Accharā kāmavaṇṇinīhamasmi;",
  "vv33:101.3_0": "Accharāsahassassāhaṁ,",
  "vv33:101.4_0": "Pavarā passa puññānaṁ vipākaṁ.",
  "vv33:102.1_0": "Tena metādiso vaṇṇo,",
  "vv33:102.2_0": "…pe…",
  "vv33:102.3_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv33:103.1_0": "“Svāgataṁ vata me ajja,",
  "vv33:103.2_0": "suppabhātaṁ suhuṭṭhitaṁ;",
  "vv33:103.3_0": "Yaṁ addasāmi devatāyo,",
  "vv33:103.4_0": "accharā kāmavaṇṇiniyo.",
  "vv33:104.1_0": "Imāsāhaṁ dhammaṁ sutvā,",
  "vv33:104.2_0": "Kāhāmi kusalaṁ bahuṁ;",
  "vv33:104.3_0": "Dānena samacariyāya,",
  "vv33:104.4_0": "Saññamena damena ca;",
  "vv33:104.5_0": "Svāhaṁ tattha gamissāmi,",
  "vv33:104.6_0": "Yattha gantvā na socare”ti.",
  "vv33:105.1_0": "Guttilavimānaṁ pañcamaṁ."
}