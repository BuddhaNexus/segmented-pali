{
  "ja278:0.1_0": "Jātaka",
  "ja278:0.2_0": "Tikanipāta",
  "ja278:0.3_0": "Udapānavagga",
  "ja278:0.4_0": "8. Mahiṁsarājajātaka",
  "ja278:1.1_0": "“Kimatthamabhisandhāya,",
  "ja278:1.2_0": "lahucittassa dubbhino;",
  "ja278:1.3_0": "Sabbakāmadadasseva,",
  "ja278:1.4_0": "imaṁ dukkhaṁ titikkhasi.",
  "ja278:2.1_0": "Siṅgena nihanāhetaṁ,",
  "ja278:2.2_0": "padasā ca adhiṭṭhaha;",
  "ja278:2.3_0": "Bhiyyo bālā pakujjheyyuṁ,",
  "ja278:2.4_0": "no cassa paṭisedhako”.",
  "ja278:3.1_0": "“Mamevāyaṁ maññamāno,",
  "ja278:3.2_0": "aññepevaṁ karissati;",
  "ja278:3.3_0": "Te naṁ tattha vadhissanti,",
  "ja278:3.4_0": "sā me mutti bhavissatī”ti.",
  "ja278:4.1_0": "Mahiṁsarājajātakaṁ aṭṭhamaṁ."
}