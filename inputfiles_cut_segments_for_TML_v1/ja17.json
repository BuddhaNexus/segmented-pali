{
  "ja17:0.1_0": "Jātaka",
  "ja17:0.2_0": "Ekakanipāta",
  "ja17:0.3_0": "Sīlavagga",
  "ja17:0.4_0": "7. Mālutajātaka",
  "ja17:1.1_0": "“Kāḷe vā yadi vā juṇhe,",
  "ja17:1.2_0": "yadā vāyati māluto;",
  "ja17:1.3_0": "Vātajāni hi sītāni,",
  "ja17:1.4_0": "ubhotthamaparājitā”ti.",
  "ja17:2.1_0": "Mālutajātakaṁ sattamaṁ."
}