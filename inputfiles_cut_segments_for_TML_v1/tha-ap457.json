{
  "tha-ap457:0.1_0": "Therāpadāna",
  "tha-ap457:0.2_0": "Jagatidāyakavagga",
  "tha-ap457:0.3_0": "5. Akkamanadāyakattheraapadāna",
  "tha-ap457:1.1_0": "“Kakusandhassa munino,",
  "tha-ap457:1.2_0": "brāhmaṇassa vusīmato;",
  "tha-ap457:1.3_0": "Divāvihāraṁ vajato,",
  "tha-ap457:1.4_0": "akkamanamadāsahaṁ.",
  "tha-ap457:2.1_0": "Imasmiṁyeva kappamhi,",
  "tha-ap457:2.2_0": "yaṁ dānamadadiṁ tadā;",
  "tha-ap457:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap457:2.4_0": "akkamanassidaṁ phalaṁ.",
  "tha-ap457:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap457:3.2_0": "…pe…",
  "tha-ap457:3.3_0": "viharāmi anāsavo.",
  "tha-ap457:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap457:4.2_0": "…pe…",
  "tha-ap457:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap457:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap457:5.2_0": "…pe…",
  "tha-ap457:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap457:6.1_0": "Itthaṁ sudaṁ āyasmā akkamanadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap457:7.1_0": "Akkamanadāyakattherassāpadānaṁ pañcamaṁ."
}