{
  "anya-e0605n3:0_0": "Anaññasādhāraṇadīpanīgāthā",
  "anya-e0605n3:1_0": "7.",
  "anya-e0605n3:2_0": "Abbhuggatā yassa guṇā anantā,",
  "anya-e0605n3:3_0": "Tibuddhakhettekadivākaroti;",
  "anya-e0605n3:4_0": "Jānāti so lokamimaṁ parañca,",
  "anya-e0605n3:5_0": "Sacetanañceva acetanañca;",
  "anya-e0605n3:6_0": "Sakassa santānagataṁ paresaṁ,",
  "anya-e0605n3:7_0": "Byatītamappattakamatrabhūtaṁ.",
  "anya-e0605n3:8_0": "8.",
  "anya-e0605n3:9_0": "Anantasattesu ca lokadhātusu,",
  "anya-e0605n3:10_0": "Ekova sabbepi samā na tena;",
  "anya-e0605n3:11_0": "Disāsu pubbādisu cakkavāḷā,",
  "anya-e0605n3:12_0": "Sahassasaṅkhāyapi appameyyā;",
  "anya-e0605n3:13_0": "Ye tesu devā manujā ca brahmā,",
  "anya-e0605n3:14_0": "Ekattha saṅgamma hi mantayantā.",
  "anya-e0605n3:15_0": "9.",
  "anya-e0605n3:16_0": "Anādikālāgatanāmarūpinaṁ,",
  "anya-e0605n3:17_0": "Yathāsakaṁ hetuphalattavuttinaṁ;",
  "anya-e0605n3:18_0": "Tabbhāvabhāvittamasambhuṇantā,",
  "anya-e0605n3:19_0": "Nānāvipallāsamanupaviṭṭhā.",
  "anya-e0605n3:20_0": "10.",
  "anya-e0605n3:21_0": "Kammappavattiñca phalappavattiṁ,",
  "anya-e0605n3:22_0": "Ekattanānattanirīhadhammataṁ;",
  "anya-e0605n3:23_0": "Viññattisantānaghanena channato,",
  "anya-e0605n3:24_0": "Sivañjasaṁ no bhaṇituṁ samatthā.",
  "anya-e0605n3:25_0": "11.",
  "anya-e0605n3:26_0": "Eko va so santikaro pabhaṅkaro,",
  "anya-e0605n3:27_0": "Saṅkhāya ñeyyāni asesitāni;",
  "anya-e0605n3:28_0": "Tesañhi majjhe paramāsambhīvadaṁ,",
  "anya-e0605n3:29_0": "Sivañjasaṁ dīpayituṁ samattho.",
  "anya-e0605n3:30_0": "12.",
  "anya-e0605n3:31_0": "So gotamo sakyasuto munindo,",
  "anya-e0605n3:32_0": "Sabbassa lokassa padīpabhūto;",
  "anya-e0605n3:33_0": "Anantasatte bhavabandhanamhā,",
  "anya-e0605n3:34_0": "Mocesi kāruññaphalānupekkhī."
}