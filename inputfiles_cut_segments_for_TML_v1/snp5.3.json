{
  "snp5.3:0.1_0": "Sutta Nipāta 5.3",
  "snp5.3:0.2_0": "Tissametteyyamāṇavapucchā",
  "snp5.3:1.1_0": "“Kodha santusito loke,",
  "snp5.3:1.2_0": "(iccāyasmā tissametteyyo)",
  "snp5.3:1.3_0": "Kassa no santi iñjitā;",
  "snp5.3:1.4_0": "Ko ubhantamabhiññāya,",
  "snp5.3:1.5_0": "Majjhe mantā na lippati;",
  "snp5.3:1.6_0": "Kaṁ brūsi mahāpurisoti,",
  "snp5.3:1.7_0": "Ko idha sibbinimaccagā”.",
  "snp5.3:2.1_0": "“Kāmesu brahmacariyavā,",
  "snp5.3:2.2_0": "(metteyyāti bhagavā)",
  "snp5.3:2.3_0": "Vītataṇho sadā sato;",
  "snp5.3:2.4_0": "Saṅkhāya nibbuto bhikkhu,",
  "snp5.3:2.5_0": "Tassa no santi iñjitā.",
  "snp5.3:3.1_0": "So ubhantamabhiññāya,",
  "snp5.3:3.2_0": "Majjhe mantā na lippati;",
  "snp5.3:3.3_0": "Taṁ brūmi mahāpurisoti,",
  "snp5.3:3.4_0": "So idha sibbinimaccagā”ti.",
  "snp5.3:3.5_0": "Tissametteyyamāṇavapucchā dutiyā."
}