{
  "iti80:0.1_0": "Itivuttaka 80",
  "iti80:0.2_0": "Tikanipāta",
  "iti80:0.3_0": "Catutthavagga",
  "iti80:0.4_0": "Vitakkasutta",
  "iti80:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti80:2.1_0": "“Tayome, bhikkhave, akusalavitakkā.",
  "iti80:2.2_0": "Katame tayo?",
  "iti80:2.3_0": "Anavaññattipaṭisaṁyutto vitakko, lābhasakkārasilokapaṭisaṁyutto vitakko, parānuddayatāpaṭisaṁyutto vitakko.",
  "iti80:2.4_0": "Ime kho, bhikkhave, tayo akusalavitakkā”ti.",
  "iti80:2.5_0": "Etamatthaṁ bhagavā avoca.",
  "iti80:2.6_0": "Tatthetaṁ iti vuccati:",
  "iti80:3.1_0": "“Anavaññattisaṁyutto,",
  "iti80:3.2_0": "lābhasakkāragāravo;",
  "iti80:3.3_0": "Sahanandī amaccehi,",
  "iti80:3.4_0": "ārā saṁyojanakkhayā.",
  "iti80:4.1_0": "Yo ca puttapasuṁ hitvā,",
  "iti80:4.2_0": "Vivāhe saṁharāni ca;",
  "iti80:4.3_0": "Bhabbo so tādiso bhikkhu,",
  "iti80:4.4_0": "Phuṭṭhuṁ sambodhimuttaman”ti.",
  "iti80:5.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti80:5.2_0": "Paṭhamaṁ."
}