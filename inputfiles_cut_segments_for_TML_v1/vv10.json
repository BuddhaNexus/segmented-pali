{
  "vv10:0.1_0": "Vimānavatthu",
  "vv10:0.2_0": "Itthivimāna",
  "vv10:0.3_0": "Pīṭhavagga",
  "vv10:0.4_0": "10. Tiladakkhiṇavimānavatthu",
  "vv10:1.1_0": "“Abhikkantena vaṇṇena,",
  "vv10:1.2_0": "yā tvaṁ tiṭṭhasi devate;",
  "vv10:1.3_0": "Obhāsentī disā sabbā,",
  "vv10:1.4_0": "osadhī viya tārakā.",
  "vv10:2.1_0": "Kena tetādiso vaṇṇo,",
  "vv10:2.2_0": "kena te idha mijjhati;",
  "vv10:2.3_0": "Uppajjanti ca te bhogā,",
  "vv10:2.4_0": "ye keci manaso piyā.",
  "vv10:3.1_0": "Pucchāmi taṁ devi mahānubhāve,",
  "vv10:3.2_0": "Manussabhūtā kimakāsi puññaṁ;",
  "vv10:3.3_0": "Kenāsi evaṁ jalitānubhāvā,",
  "vv10:3.4_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv10:4.1_0": "Sā devatā attamanā,",
  "vv10:4.2_0": "moggallānena pucchitā;",
  "vv10:4.3_0": "Pañhaṁ puṭṭhā viyākāsi,",
  "vv10:4.4_0": "yassa kammassidaṁ phalaṁ.",
  "vv10:5.1_0": "“Ahaṁ manussesu manussabhūtā,",
  "vv10:5.2_0": "Purimāya jātiyā manussaloke.",
  "vv10:6.1_0": "Addasaṁ virajaṁ buddhaṁ,",
  "vv10:6.2_0": "vippasannamanāvilaṁ;",
  "vv10:6.3_0": "Āsajja dānaṁ adāsiṁ,",
  "vv10:6.4_0": "akāmā tiladakkhiṇaṁ;",
  "vv10:6.5_0": "Dakkhiṇeyyassa buddhassa,",
  "vv10:6.6_0": "pasannā sehi pāṇibhi.",
  "vv10:7.1_0": "Tena metādiso vaṇṇo,",
  "vv10:7.2_0": "tena me idha mijjhati;",
  "vv10:7.3_0": "Uppajjanti ca me bhogā,",
  "vv10:7.4_0": "ye keci manaso piyā.",
  "vv10:8.1_0": "Akkhāmi te bhikkhu mahānubhāva,",
  "vv10:8.2_0": "Manussabhūtā yamakāsi puññaṁ;",
  "vv10:8.3_0": "Tenamhi evaṁ jalitānubhāvā,",
  "vv10:8.4_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv10:9.1_0": "Tiladakkhiṇavimānaṁ dasamaṁ."
}