{
  "ja60:0.1_0": "Jātaka",
  "ja60:0.2_0": "Ekakanipāta",
  "ja60:0.3_0": "Āsīsavagga",
  "ja60:0.4_0": "10. Saṅkhadhamajātaka",
  "ja60:1.1_0": "“Dhame dhame nātidhame,",
  "ja60:1.2_0": "atidhantañhi pāpakaṁ;",
  "ja60:1.3_0": "Dhantenādhigatā bhogā,",
  "ja60:1.4_0": "te tāto vidhamī dhaman”ti.",
  "ja60:2.1_0": "Saṅkhadhamajātakaṁ dasamaṁ.",
  "ja60:2.2_0": "Āsīsavaggo chaṭṭho.",
  "ja60:3.1_0": "Tassuddānaṁ",
  "ja60:4.1_0": "Yathā icchiṁ tathāhudakā thalā,",
  "ja60:4.2_0": "Sura sāduphalo ca alīnamano;",
  "ja60:4.3_0": "Sampahaṭṭhamano caturo ca tayo,",
  "ja60:4.4_0": "Sataladdhaka bhogadhanena dasāti."
}