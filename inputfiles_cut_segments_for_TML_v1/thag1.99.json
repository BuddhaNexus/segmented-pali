{
  "thag1.99:0.1_0": "Theragāthā",
  "thag1.99:0.2_0": "Ekakanipāta",
  "thag1.99:0.3_0": "Dasamavagga",
  "thag1.99:0.4_0": "1.99. Uttiyattheragāthā",
  "thag1.99:1.1_0": "“Saddaṁ sutvā sati muṭṭhā,",
  "thag1.99:1.2_0": "Piyaṁ nimittaṁ manasikaroto;",
  "thag1.99:1.3_0": "Sārattacitto vedeti,",
  "thag1.99:1.4_0": "Tañca ajjhosa tiṭṭhati;",
  "thag1.99:1.5_0": "Tassa vaḍḍhanti āsavā,",
  "thag1.99:1.6_0": "Saṁsāraṁ upagāmino”ti.",
  "thag1.99:1.7_0": "… Uttiyo thero …."
}