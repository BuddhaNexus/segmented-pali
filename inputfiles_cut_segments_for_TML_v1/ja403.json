{
  "ja403:0.1_0": "Jātaka",
  "ja403:0.2_0": "Sattakanipāta",
  "ja403:0.3_0": "Kukkuvagga",
  "ja403:0.4_0": "8. Aṭṭhisenakajātaka",
  "ja403:1.1_0": "“Yeme ahaṁ na jānāmi,",
  "ja403:1.2_0": "aṭṭhisena vanibbake;",
  "ja403:1.3_0": "Te maṁ saṅgamma yācanti,",
  "ja403:1.4_0": "kasmā maṁ tvaṁ na yācasi”.",
  "ja403:2.1_0": "“Yācako appiyo hoti,",
  "ja403:2.2_0": "yācaṁ adadamappiyo;",
  "ja403:2.3_0": "Tasmāhaṁ taṁ na yācāmi,",
  "ja403:2.4_0": "mā me videssanā ahu”.",
  "ja403:3.1_0": "“Yo ve yācanajīvāno,",
  "ja403:3.2_0": "kāle yācaṁ na yācati;",
  "ja403:3.3_0": "Parañca puññā dhaṁseti,",
  "ja403:3.4_0": "attanāpi na jīvati.",
  "ja403:4.1_0": "Yo ca yācanajīvāno,",
  "ja403:4.2_0": "kāle yācañhi yācati;",
  "ja403:4.3_0": "Parañca puññaṁ labbheti,",
  "ja403:4.4_0": "attanāpi ca jīvati.",
  "ja403:5.1_0": "Na ve dessanti sappaññā,",
  "ja403:5.2_0": "Disvā yācakamāgate;",
  "ja403:5.3_0": "Brahmacāri piyo mesi,",
  "ja403:5.4_0": "Vada tvaṁ bhaññamicchasi”.",
  "ja403:6.1_0": "“Na ve yācanti sappaññā,",
  "ja403:6.2_0": "Dhīro ca veditumarahati;",
  "ja403:6.3_0": "Uddissa ariyā tiṭṭhanti,",
  "ja403:6.4_0": "Esā ariyāna yācanā”.",
  "ja403:7.1_0": "“Dadāmi te brāhmaṇa rohiṇīnaṁ,",
  "ja403:7.2_0": "Gavaṁ sahassaṁ saha puṅgavena;",
  "ja403:7.3_0": "Ariyo hi ariyassa kathaṁ na dajjā,",
  "ja403:7.4_0": "Sutvāna gāthā tava dhammayuttā”ti.",
  "ja403:8.1_0": "Aṭṭhisenakajātakaṁ aṭṭhamaṁ."
}