{
  "ja82:0.1_0": "Jātaka",
  "ja82:0.2_0": "Ekakanipāta",
  "ja82:0.3_0": "Apāyimhavagga",
  "ja82:0.4_0": "2. Mittavindakajātaka",
  "ja82:1.1_0": "“Atikkamma ramaṇakaṁ,",
  "ja82:1.2_0": "sadāmattañca dūbhakaṁ;",
  "ja82:1.3_0": "Svāsi pāsāṇamāsīno,",
  "ja82:1.4_0": "yasmā jīvaṁ na mokkhasī”ti.",
  "ja82:2.1_0": "Mittavindakajātakaṁ dutiyaṁ."
}