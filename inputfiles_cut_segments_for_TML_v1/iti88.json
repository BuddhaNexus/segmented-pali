{
  "iti88:0.1_0": "Itivuttaka 88",
  "iti88:0.2_0": "Tikanipāta",
  "iti88:0.3_0": "Catutthavagga",
  "iti88:0.4_0": "Antarāmalasutta",
  "iti88:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti88:2.1_0": "“Tayome, bhikkhave, antarāmalā antarāamittā antarāsapattā antarāvadhakā antarāpaccatthikā.",
  "iti88:2.2_0": "Katame tayo?",
  "iti88:2.3_0": "Lobho, bhikkhave, antarāmalo antarāamitto antarāsapatto antarāvadhako antarāpaccatthiko.",
  "iti88:2.4_0": "Doso, bhikkhave, antarāmalo antarāamitto antarāsapatto antarāvadhako antarāpaccatthiko.",
  "iti88:2.5_0": "Moho, bhikkhave, antarāmalo antarāamitto antarāsapatto antarāvadhako antarāpaccatthiko.",
  "iti88:2.6_0": "Ime kho, bhikkhave, tayo antarāmalā antarāamittā antarāsapattā antarāvadhakā antarāpaccatthikā”ti.",
  "iti88:2.7_0": "Etamatthaṁ bhagavā avoca.",
  "iti88:2.8_0": "Tatthetaṁ iti vuccati:",
  "iti88:3.1_0": "“Anatthajanano lobho,",
  "iti88:3.2_0": "lobho cittappakopano;",
  "iti88:3.3_0": "Bhayamantarato jātaṁ,",
  "iti88:3.4_0": "taṁ jano nāvabujjhati.",
  "iti88:4.1_0": "Luddho atthaṁ na jānāti,",
  "iti88:4.2_0": "luddho dhammaṁ na passati;",
  "iti88:4.3_0": "Andhatamaṁ tadā hoti,",
  "iti88:4.4_0": "yaṁ lobho sahate naraṁ.",
  "iti88:5.1_0": "Yo ca lobhaṁ pahantvāna,",
  "iti88:5.2_0": "lobhaneyye na lubbhati;",
  "iti88:5.3_0": "Lobho pahīyate tamhā,",
  "iti88:5.4_0": "udabindūva pokkharā.",
  "iti88:6.1_0": "Anatthajanano doso,",
  "iti88:6.2_0": "doso cittappakopano;",
  "iti88:6.3_0": "Bhayamantarato jātaṁ,",
  "iti88:6.4_0": "taṁ jano nāvabujjhati.",
  "iti88:7.1_0": "Duṭṭho atthaṁ na jānāti,",
  "iti88:7.2_0": "duṭṭho dhammaṁ na passati;",
  "iti88:7.3_0": "Andhatamaṁ tadā hoti,",
  "iti88:7.4_0": "yaṁ doso sahate naraṁ.",
  "iti88:8.1_0": "Yo ca dosaṁ pahantvāna,",
  "iti88:8.2_0": "dosaneyye na dussati;",
  "iti88:8.3_0": "Doso pahīyate tamhā,",
  "iti88:8.4_0": "tālapakkaṁva bandhanā.",
  "iti88:9.1_0": "Anatthajanano moho,",
  "iti88:9.2_0": "moho cittappakopano;",
  "iti88:9.3_0": "Bhayamantarato jātaṁ,",
  "iti88:9.4_0": "taṁ jano nāvabujjhati.",
  "iti88:10.1_0": "Mūḷho atthaṁ na jānāti,",
  "iti88:10.2_0": "mūḷho dhammaṁ na passati;",
  "iti88:10.3_0": "Andhatamaṁ tadā hoti,",
  "iti88:10.4_0": "yaṁ moho sahate naraṁ.",
  "iti88:11.1_0": "Yo ca mohaṁ pahantvāna,",
  "iti88:11.2_0": "mohaneyye na muyhati;",
  "iti88:11.3_0": "Mohaṁ vihanti so sabbaṁ,",
  "iti88:11.4_0": "ādiccovudayaṁ taman”ti.",
  "iti88:12.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti88:12.2_0": "Navamaṁ."
}