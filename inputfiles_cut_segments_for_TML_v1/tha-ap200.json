{
  "tha-ap200:0.1_0": "Therāpadāna",
  "tha-ap200:0.2_0": "Tamālapupphiyavagga",
  "tha-ap200:0.3_0": "8 Muṭṭhipūjakattheraapadāna",
  "tha-ap200:1.1_0": "“Sumedho nāma bhagavā,",
  "tha-ap200:1.2_0": "lokajeṭṭho narāsabho;",
  "tha-ap200:1.3_0": "Pacchime anukampāya,",
  "tha-ap200:1.4_0": "padhānaṁ padahī jino.",
  "tha-ap200:2.1_0": "Tassa caṅkamamānassa,",
  "tha-ap200:2.2_0": "dvipadindassa tādino;",
  "tha-ap200:2.3_0": "Girinelassa pupphānaṁ,",
  "tha-ap200:2.4_0": "muṭṭhiṁ buddhassa ropayiṁ.",
  "tha-ap200:3.1_0": "Tena cittappasādena,",
  "tha-ap200:3.2_0": "sukkamūlena codito;",
  "tha-ap200:3.3_0": "Tiṁsakappasahassāni,",
  "tha-ap200:3.4_0": "duggatiṁ nupapajjahaṁ.",
  "tha-ap200:4.1_0": "Tevīsatikappasate,",
  "tha-ap200:4.2_0": "sunelo nāma khattiyo;",
  "tha-ap200:4.3_0": "Sattaratanasampanno,",
  "tha-ap200:4.4_0": "eko āsiṁ mahabbalo.",
  "tha-ap200:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap200:5.2_0": "…pe…",
  "tha-ap200:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap200:6.1_0": "Itthaṁ sudaṁ āyasmā muṭṭhipūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap200:7.1_0": "Muṭṭhipūjakattherassāpadānaṁ aṭṭhamaṁ."
}