{
  "thag2.26:0.1_0": "Theragāthā",
  "thag2.26:0.2_0": "Dukanipāta",
  "thag2.26:0.3_0": "Tatiyavagga",
  "thag2.26:0.4_0": "2.26. Puṇṇamāsattheragāthā",
  "thag2.26:1.1_0": "“Pañca nīvaraṇe hitvā,",
  "thag2.26:1.2_0": "yogakkhemassa pattiyā;",
  "thag2.26:1.3_0": "Dhammādāsaṁ gahetvāna,",
  "thag2.26:1.4_0": "ñāṇadassanamattano.",
  "thag2.26:2.1_0": "Paccavekkhiṁ imaṁ kāyaṁ,",
  "thag2.26:2.2_0": "sabbaṁ santarabāhiraṁ;",
  "thag2.26:2.3_0": "Ajjhattañca bahiddhā ca,",
  "thag2.26:2.4_0": "tuccho kāyo adissathā”ti.",
  "thag2.26:2.5_0": "… Puṇṇamāso thero …."
}