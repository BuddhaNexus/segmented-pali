{
  "ja390:0.1_0": "Jātaka",
  "ja390:0.2_0": "Chakkanipāta",
  "ja390:0.3_0": "Kharaputtavagga",
  "ja390:0.4_0": "5. Mayhakajātaka",
  "ja390:1.1_0": "“Sakuṇo mayhako nāma,",
  "ja390:1.2_0": "girisānudarīcaro;",
  "ja390:1.3_0": "Pakkaṁ pipphalimāruyha,",
  "ja390:1.4_0": "mayhaṁ mayhanti kandati.",
  "ja390:2.1_0": "Tassevaṁ vilapantassa,",
  "ja390:2.2_0": "dijasaṅghā samāgatā;",
  "ja390:2.3_0": "Bhutvāna pipphaliṁ yanti,",
  "ja390:2.4_0": "vilapatveva so dijo.",
  "ja390:3.1_0": "Evameva idhekacco,",
  "ja390:3.2_0": "saṅgharitvā bahuṁ dhanaṁ;",
  "ja390:3.3_0": "Nevattano na ñātīnaṁ,",
  "ja390:3.4_0": "yathodhiṁ paṭipajjati.",
  "ja390:4.1_0": "Na so acchādanaṁ bhattaṁ,",
  "ja390:4.2_0": "na mālaṁ na vilepanaṁ;",
  "ja390:4.3_0": "Anubhoti sakiṁ kiñci,",
  "ja390:4.4_0": "na saṅgaṇhāti ñātake.",
  "ja390:5.1_0": "Tassevaṁ vilapantassa,",
  "ja390:5.2_0": "mayhaṁ mayhanti rakkhato;",
  "ja390:5.3_0": "Rājāno atha vā corā,",
  "ja390:5.4_0": "dāyādā ye va appiyā;",
  "ja390:5.5_0": "Dhanamādāya gacchanti,",
  "ja390:5.6_0": "vilapatveva so naro.",
  "ja390:6.1_0": "Dhīro bhoge adhigamma,",
  "ja390:6.2_0": "saṅgaṇhāti ca ñātake;",
  "ja390:6.3_0": "Tena so kittiṁ pappoti,",
  "ja390:6.4_0": "pecca sagge pamodatī”ti.",
  "ja390:7.1_0": "Mayhakajātakaṁ pañcamaṁ."
}