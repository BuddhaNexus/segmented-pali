{
  "ja154:0.1_0": "Jātaka",
  "ja154:0.2_0": "Dukanipāta",
  "ja154:0.3_0": "Daḷhavagga",
  "ja154:0.4_0": "4. Uragajātaka",
  "ja154:1.1_0": "“Idhūragānaṁ pavaro paviṭṭho,",
  "ja154:1.2_0": "Selassa vaṇṇena pamokkhamicchaṁ;",
  "ja154:1.3_0": "Brahmañca vaṇṇaṁ apacāyamāno,",
  "ja154:1.4_0": "Bubhukkhito no vitarāmi bhottuṁ”.",
  "ja154:2.1_0": "“So brahmagutto cirameva jīva,",
  "ja154:2.2_0": "Dibyā ca te pātubhavantu bhakkhā;",
  "ja154:2.3_0": "Yo brahmavaṇṇaṁ apacāyamāno,",
  "ja154:2.4_0": "Bubhukkhito no vitarāsi bhottun”ti.",
  "ja154:3.1_0": "Uragajātakaṁ catutthaṁ."
}