{
  "ja157:0.1_0": "Jātaka",
  "ja157:0.2_0": "Dukanipāta",
  "ja157:0.3_0": "Daḷhavagga",
  "ja157:0.4_0": "7. Guṇajātaka",
  "ja157:1.1_0": "“Yena kāmaṁ paṇāmeti,",
  "ja157:1.2_0": "dhammo balavataṁ migī;",
  "ja157:1.3_0": "Unnadantī vijānāhi,",
  "ja157:1.4_0": "jātaṁ saraṇato bhayaṁ”.",
  "ja157:2.1_0": "“Api cepi dubbalo mitto,",
  "ja157:2.2_0": "mittadhammesu tiṭṭhati;",
  "ja157:2.3_0": "So ñātako ca bandhu ca,",
  "ja157:2.4_0": "so mitto so ca me sakhā;",
  "ja157:2.5_0": "Dāṭhini mātimaññittho,",
  "ja157:2.6_0": "siṅgālo mama pāṇado”ti.",
  "ja157:3.1_0": "Guṇajātakaṁ sattamaṁ."
}