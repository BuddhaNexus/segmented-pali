{
  "snp5.16:0.1_0": "Sutta Nipāta 5.16",
  "snp5.16:0.2_0": "Mogharājamāṇavapucchā",
  "snp5.16:1.1_0": "“Dvāhaṁ sakkaṁ apucchissaṁ,",
  "snp5.16:1.2_0": "(iccāyasmā mogharājā)",
  "snp5.16:1.3_0": "Na me byākāsi cakkhumā;",
  "snp5.16:1.4_0": "Yāvatatiyañca devīsi,",
  "snp5.16:1.5_0": "Byākarotīti me sutaṁ.",
  "snp5.16:2.1_0": "Ayaṁ loko paro loko,",
  "snp5.16:2.2_0": "brahmaloko sadevako;",
  "snp5.16:2.3_0": "Diṭṭhiṁ te nābhijānāti,",
  "snp5.16:2.4_0": "gotamassa yasassino.",
  "snp5.16:3.1_0": "Evaṁ abhikkantadassāviṁ,",
  "snp5.16:3.2_0": "atthi pañhena āgamaṁ;",
  "snp5.16:3.3_0": "Kathaṁ lokaṁ avekkhantaṁ,",
  "snp5.16:3.4_0": "maccurājā na passati”.",
  "snp5.16:4.1_0": "“Suññato lokaṁ avekkhassu,",
  "snp5.16:4.2_0": "Mogharāja sadā sato;",
  "snp5.16:4.3_0": "Attānudiṭṭhiṁ ūhacca,",
  "snp5.16:4.4_0": "Evaṁ maccutaro siyā;",
  "snp5.16:4.5_0": "Evaṁ lokaṁ avekkhantaṁ,",
  "snp5.16:4.6_0": "Maccurājā na passatī”ti.",
  "snp5.16:4.7_0": "Mogharājamāṇavapucchā pannarasamā."
}