{
  "thag1.70:0.1_0": "Theragāthā",
  "thag1.70:0.2_0": "Ekakanipāta",
  "thag1.70:0.3_0": "Sattamavagga",
  "thag1.70:0.4_0": "1.70. Puṇṇattheragāthā",
  "thag1.70:1.1_0": "“Sīlameva idha aggaṁ,",
  "thag1.70:1.2_0": "paññavā pana uttamo;",
  "thag1.70:1.3_0": "Manussesu ca devesu,",
  "thag1.70:1.4_0": "sīlapaññāṇato jayan”ti.",
  "thag1.70:1.5_0": "…",
  "thag1.70:1.6_0": "Puṇṇo thero ….",
  "thag1.70:1.7_0": "vaggo sattamo.",
  "thag1.70:2.0_0": "Tassuddānaṁ",
  "thag1.70:2.1_0": "Vappo ca vajjiputto ca,",
  "thag1.70:2.2_0": "pakkho vimalakoṇḍañño;",
  "thag1.70:2.3_0": "Ukkhepakatavaccho ca,",
  "thag1.70:2.4_0": "meghiyo ekadhammiko;",
  "thag1.70:2.5_0": "Ekudāniyachannā ca,",
  "thag1.70:2.6_0": "puṇṇatthero mahabbaloti."
}