{
  "tha-ap503:0.1_0": "Therāpadāna",
  "tha-ap503:0.2_0": "Kaṇikāravagga",
  "tha-ap503:0.3_0": "1 Tikaṇikārapupphiyattheraapadāna",
  "tha-ap503:1.1_0": "“Sumedho nāma sambuddho,",
  "tha-ap503:1.2_0": "bāttiṁsavaralakkhaṇo;",
  "tha-ap503:1.3_0": "Vivekakāmo sambuddho,",
  "tha-ap503:1.4_0": "himavantamupāgamiṁ.",
  "tha-ap503:2.1_0": "Ajjhogayha himavantaṁ,",
  "tha-ap503:2.2_0": "aggo kāruṇiko muni;",
  "tha-ap503:2.3_0": "Pallaṅkamābhujitvāna,",
  "tha-ap503:2.4_0": "nisīdi purisuttamo.",
  "tha-ap503:3.1_0": "Vijjādharo tadā āsiṁ,",
  "tha-ap503:3.2_0": "antalikkhacaro ahaṁ;",
  "tha-ap503:3.3_0": "Tisūlaṁ sukataṁ gayha,",
  "tha-ap503:3.4_0": "gacchāmi ambare tadā.",
  "tha-ap503:4.1_0": "Pabbatagge yathā aggi,",
  "tha-ap503:4.2_0": "puṇṇamāyeva candimā;",
  "tha-ap503:4.3_0": "Vane obhāsate buddho,",
  "tha-ap503:4.4_0": "sālarājāva phullito.",
  "tha-ap503:5.1_0": "Vanaggā nikkhamitvāna,",
  "tha-ap503:5.2_0": "buddharaṁsībhidhāvare;",
  "tha-ap503:5.3_0": "Naḷaggivaṇṇasaṅkāsā,",
  "tha-ap503:5.4_0": "disvā cittaṁ pasādayiṁ.",
  "tha-ap503:6.1_0": "Vicinaṁ addasaṁ pupphaṁ,",
  "tha-ap503:6.2_0": "kaṇikāraṁ devagandhikaṁ;",
  "tha-ap503:6.3_0": "Tīṇi pupphāni ādāya,",
  "tha-ap503:6.4_0": "buddhaseṭṭhamapūjayiṁ.",
  "tha-ap503:7.1_0": "Buddhassa ānubhāvena,",
  "tha-ap503:7.2_0": "tīṇi pupphāni me tadā;",
  "tha-ap503:7.3_0": "Uddhaṁvaṇṭā adhopattā,",
  "tha-ap503:7.4_0": "chāyaṁ kubbanti satthuno.",
  "tha-ap503:8.1_0": "Tena kammena sukatena,",
  "tha-ap503:8.2_0": "cetanāpaṇidhīhi ca;",
  "tha-ap503:8.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "tha-ap503:8.4_0": "tāvatiṁsamagacchahaṁ.",
  "tha-ap503:9.1_0": "Tattha me sukataṁ byamhaṁ,",
  "tha-ap503:9.2_0": "kaṇikārīti ñāyati;",
  "tha-ap503:9.3_0": "Saṭṭhiyojanamubbedhaṁ,",
  "tha-ap503:9.4_0": "tiṁsayojanavitthataṁ.",
  "tha-ap503:10.1_0": "Sahassakaṇḍaṁ satabheṇḍu,",
  "tha-ap503:10.2_0": "dhajāluharitāmayaṁ;",
  "tha-ap503:10.3_0": "Satasahassaniyyūhā,",
  "tha-ap503:10.4_0": "byamhe pātubhaviṁsu me.",
  "tha-ap503:11.1_0": "Soṇṇamayā maṇimayā,",
  "tha-ap503:11.2_0": "lohitaṅgamayāpi ca;",
  "tha-ap503:11.3_0": "Phalikāpi ca pallaṅkā,",
  "tha-ap503:11.4_0": "yenicchakā yadicchakā.",
  "tha-ap503:12.1_0": "Mahārahañca sayanaṁ,",
  "tha-ap503:12.2_0": "tūlikāvikatīyutaṁ;",
  "tha-ap503:12.3_0": "Uddhalomikaekantaṁ,",
  "tha-ap503:12.4_0": "bibbohanasamāyutaṁ.",
  "tha-ap503:13.1_0": "Bhavanā nikkhamitvāna,",
  "tha-ap503:13.2_0": "caranto devacārikaṁ;",
  "tha-ap503:13.3_0": "Yadā icchāmi gamanaṁ,",
  "tha-ap503:13.4_0": "devasaṅghapurakkhato.",
  "tha-ap503:14.1_0": "Pupphassa heṭṭhā tiṭṭhāmi,",
  "tha-ap503:14.2_0": "uparicchadanaṁ mama;",
  "tha-ap503:14.3_0": "Samantā yojanasataṁ,",
  "tha-ap503:14.4_0": "kaṇikārehi chāditaṁ.",
  "tha-ap503:15.1_0": "Saṭṭhituriyasahassāni,",
  "tha-ap503:15.2_0": "sāyaṁ pātamupaṭṭhahuṁ;",
  "tha-ap503:15.3_0": "Parivārenti maṁ niccaṁ,",
  "tha-ap503:15.4_0": "rattindivamatanditā.",
  "tha-ap503:16.1_0": "Tattha naccehi gītehi,",
  "tha-ap503:16.2_0": "tāḷehi vāditehi ca;",
  "tha-ap503:16.3_0": "Ramāmi khiḍḍāratiyā,",
  "tha-ap503:16.4_0": "modāmi kāmakāmihaṁ.",
  "tha-ap503:17.1_0": "Tattha bhutvā pivitvā ca,",
  "tha-ap503:17.2_0": "modāmi tidase tadā;",
  "tha-ap503:17.3_0": "Nārīgaṇehi sahito,",
  "tha-ap503:17.4_0": "modāmi byamhamuttame.",
  "tha-ap503:18.1_0": "Satānaṁ pañcakkhattuñca,",
  "tha-ap503:18.2_0": "devarajjamakārayiṁ;",
  "tha-ap503:18.3_0": "Satānaṁ tīṇikkhattuñca,",
  "tha-ap503:18.4_0": "cakkavattī ahosahaṁ;",
  "tha-ap503:18.5_0": "Padesarajjaṁ vipulaṁ,",
  "tha-ap503:18.6_0": "gaṇanāto asaṅkhiyaṁ.",
  "tha-ap503:19.1_0": "Bhavābhave saṁsaranto,",
  "tha-ap503:19.2_0": "mahābhogaṁ labhāmahaṁ;",
  "tha-ap503:19.3_0": "Bhoge me ūnatā natthi,",
  "tha-ap503:19.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap503:20.1_0": "Duve bhave saṁsarāmi,",
  "tha-ap503:20.2_0": "devatte atha mānuse;",
  "tha-ap503:20.3_0": "Aññaṁ gatiṁ na jānāmi,",
  "tha-ap503:20.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap503:21.1_0": "Duve kule pajāyāmi,",
  "tha-ap503:21.2_0": "khattiye cāpi brāhmaṇe;",
  "tha-ap503:21.3_0": "Nīce kule na jāyāmi,",
  "tha-ap503:21.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap503:22.1_0": "Hatthiyānaṁ assayānaṁ,",
  "tha-ap503:22.2_0": "sivikaṁ sandamānikaṁ;",
  "tha-ap503:22.3_0": "Labhāmi sabbamevetaṁ,",
  "tha-ap503:22.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap503:23.1_0": "Dāsīgaṇaṁ dāsagaṇaṁ,",
  "tha-ap503:23.2_0": "nāriyo samalaṅkatā;",
  "tha-ap503:23.3_0": "Labhāmi sabbamevetaṁ,",
  "tha-ap503:23.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap503:24.1_0": "Koseyyakambaliyāni,",
  "tha-ap503:24.2_0": "khomakappāsikāni ca;",
  "tha-ap503:24.3_0": "Labhāmi sabbamevetaṁ,",
  "tha-ap503:24.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap503:25.1_0": "Navavatthaṁ navaphalaṁ,",
  "tha-ap503:25.2_0": "navaggarasabhojanaṁ;",
  "tha-ap503:25.3_0": "Labhāmi sabbamevetaṁ,",
  "tha-ap503:25.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap503:26.1_0": "Imaṁ khāda imaṁ bhuñja,",
  "tha-ap503:26.2_0": "imamhi sayane saya;",
  "tha-ap503:26.3_0": "Labhāmi sabbamevetaṁ,",
  "tha-ap503:26.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap503:27.1_0": "Sabbattha pūjito homi,",
  "tha-ap503:27.2_0": "yaso abbhuggato mama;",
  "tha-ap503:27.3_0": "Mahāpakkho sadā homi,",
  "tha-ap503:27.4_0": "abhejjapariso sadā;",
  "tha-ap503:27.5_0": "Ñātīnaṁ uttamo homi,",
  "tha-ap503:27.6_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap503:28.1_0": "Sītaṁ uṇhaṁ na jānāmi,",
  "tha-ap503:28.2_0": "pariḷāho na vijjati;",
  "tha-ap503:28.3_0": "Atho cetasikaṁ dukkhaṁ,",
  "tha-ap503:28.4_0": "hadaye me na vijjati.",
  "tha-ap503:29.1_0": "Suvaṇṇavaṇṇo hutvāna,",
  "tha-ap503:29.2_0": "saṁsarāmi bhavābhave;",
  "tha-ap503:29.3_0": "Vevaṇṇiyaṁ na jānāmi,",
  "tha-ap503:29.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap503:30.1_0": "Devalokā cavitvāna,",
  "tha-ap503:30.2_0": "sukkamūlena codito;",
  "tha-ap503:30.3_0": "Sāvatthiyaṁ pure jāto,",
  "tha-ap503:30.4_0": "mahāsāle suaḍḍhake.",
  "tha-ap503:31.1_0": "Pañca kāmaguṇe hitvā,",
  "tha-ap503:31.2_0": "pabbajiṁ anagāriyaṁ;",
  "tha-ap503:31.3_0": "Jātiyā sattavassohaṁ,",
  "tha-ap503:31.4_0": "arahattamapāpuṇiṁ.",
  "tha-ap503:32.1_0": "Upasampādayī buddho,",
  "tha-ap503:32.2_0": "guṇamaññāya cakkhumā;",
  "tha-ap503:32.3_0": "Taruṇo pūjanīyohaṁ,",
  "tha-ap503:32.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap503:33.1_0": "Dibbacakkhu visuddhaṁ me,",
  "tha-ap503:33.2_0": "samādhikusalo ahaṁ;",
  "tha-ap503:33.3_0": "Abhiññāpāramippatto,",
  "tha-ap503:33.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap503:34.1_0": "Paṭisambhidā anuppatto,",
  "tha-ap503:34.2_0": "iddhipādesu kovido;",
  "tha-ap503:34.3_0": "Dhammesu pāramippatto,",
  "tha-ap503:34.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap503:35.1_0": "Tiṁsakappasahassamhi,",
  "tha-ap503:35.2_0": "yaṁ buddhamabhipūjayiṁ;",
  "tha-ap503:35.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap503:35.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap503:36.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap503:36.2_0": "bhavā sabbe samūhatā;",
  "tha-ap503:36.3_0": "Nāgova bandhanaṁ chetvā,",
  "tha-ap503:36.4_0": "viharāmi anāsavo.",
  "tha-ap503:37.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap503:37.2_0": "mama buddhassa santike;",
  "tha-ap503:37.3_0": "Tisso vijjā anuppattā,",
  "tha-ap503:37.4_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap503:38.1_0": "Paṭisambhidā catasso,",
  "tha-ap503:38.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap503:38.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap503:38.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap503:39.1_0": "Itthaṁ sudaṁ āyasmā tikaṇikārapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap503:40.1_0": "Tikaṇikārapupphiyattherassāpadānaṁ paṭhamaṁ."
}