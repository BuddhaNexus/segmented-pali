{
  "tha-ap527:0.1_0": "Therāpadāna",
  "tha-ap527:0.2_0": "Tiṇadāyakavagga",
  "tha-ap527:0.3_0": "5. Supaṭadāyakattheraapadāna",
  "tha-ap527:1.1_0": "“Divāvihārā nikkhanto,",
  "tha-ap527:1.2_0": "vipassī lokanāyako;",
  "tha-ap527:1.3_0": "Lahuṁ supaṭakaṁ datvā,",
  "tha-ap527:1.4_0": "kappaṁ saggamhi modahaṁ.",
  "tha-ap527:2.1_0": "Ekanavutito kappe,",
  "tha-ap527:2.2_0": "supaṭakamadāsahaṁ;",
  "tha-ap527:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap527:2.4_0": "supaṭassa idaṁ phalaṁ.",
  "tha-ap527:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap527:3.2_0": "…pe…",
  "tha-ap527:3.3_0": "viharāmi anāsavo.",
  "tha-ap527:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap527:4.2_0": "…pe…",
  "tha-ap527:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap527:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap527:5.2_0": "…pe…",
  "tha-ap527:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap527:6.1_0": "Itthaṁ sudaṁ āyasmā supaṭadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap527:7.1_0": "Supaṭadāyakattherassāpadānaṁ pañcamaṁ."
}