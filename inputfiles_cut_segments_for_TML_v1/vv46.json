{
  "vv46:0.1_0": "Vimānavatthu",
  "vv46:0.2_0": "Itthivimāna",
  "vv46:0.3_0": "Mañjiṭṭhakavagga",
  "vv46:0.4_0": "8. Ambavimānavatthu",
  "vv46:1.1_0": "“Dibbaṁ te ambavanaṁ rammaṁ,",
  "vv46:1.2_0": "Pāsādettha mahallako;",
  "vv46:1.3_0": "Nānātūriyasaṅghuṭṭho,",
  "vv46:1.4_0": "Accharāgaṇaghosito.",
  "vv46:2.1_0": "Padīpo cettha jalati,",
  "vv46:2.2_0": "niccaṁ sovaṇṇayo mahā;",
  "vv46:2.3_0": "Dussaphalehi rukkhehi,",
  "vv46:2.4_0": "samantā parivārito.",
  "vv46:3.1_0": "Kena tetādiso vaṇṇo,",
  "vv46:3.2_0": "…pe…",
  "vv46:3.3_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv46:4.1_0": "Sā devatā attamanā,",
  "vv46:4.2_0": "…pe…",
  "vv46:4.3_0": "yassa kammassidaṁ phalaṁ.",
  "vv46:5.1_0": "“Ahaṁ manussesu manussabhūtā,",
  "vv46:5.2_0": "Purimāya jātiyā manussaloke;",
  "vv46:5.3_0": "Vihāraṁ saṅghassa kāresiṁ,",
  "vv46:5.4_0": "Ambehi parivāritaṁ.",
  "vv46:6.1_0": "Pariyosite vihāre,",
  "vv46:6.2_0": "kārente niṭṭhite mahe;",
  "vv46:6.3_0": "Ambehi chādayitvāna,",
  "vv46:6.4_0": "katvā dussamaye phale.",
  "vv46:7.1_0": "Padīpaṁ tattha jāletvā,",
  "vv46:7.2_0": "bhojayitvā gaṇuttamaṁ;",
  "vv46:7.3_0": "Niyyādesiṁ taṁ saṅghassa,",
  "vv46:7.4_0": "pasannā sehi pāṇibhi.",
  "vv46:8.1_0": "Tena me ambavanaṁ rammaṁ,",
  "vv46:8.2_0": "pāsādettha mahallako;",
  "vv46:8.3_0": "Nānātūriyasaṅghuṭṭho,",
  "vv46:8.4_0": "accharāgaṇaghosito.",
  "vv46:9.1_0": "Padīpo cettha jalati,",
  "vv46:9.2_0": "niccaṁ sovaṇṇayo mahā;",
  "vv46:9.3_0": "Dussaphalehi rukkhehi,",
  "vv46:9.4_0": "samantā parivārito.",
  "vv46:10.1_0": "Tena metādiso vaṇṇo,",
  "vv46:10.2_0": "…pe…",
  "vv46:10.3_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv46:11.1_0": "Ambavimānaṁ aṭṭhamaṁ."
}