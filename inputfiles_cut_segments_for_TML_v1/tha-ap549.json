{
  "tha-ap549:0.1_0": "Therāpadāna",
  "tha-ap549:0.2_0": "Bhaddiyavagga",
  "tha-ap549:0.3_0": "7. Abhayattheraapadāna",
  "tha-ap549:1.1_0": "“Padumuttaro nāma jino,",
  "tha-ap549:1.2_0": "sabbadhammesu cakkhumā;",
  "tha-ap549:1.3_0": "Ito satasahassamhi,",
  "tha-ap549:1.4_0": "kappe uppajji nāyako.",
  "tha-ap549:2.1_0": "Saraṇagamane kiñci,",
  "tha-ap549:2.2_0": "nivesesi tathāgato;",
  "tha-ap549:2.3_0": "Kiñci sīle nivesesi,",
  "tha-ap549:2.4_0": "dasakammapathuttame.",
  "tha-ap549:3.1_0": "Deti kassaci so vīro,",
  "tha-ap549:3.2_0": "sāmaññaphalamuttamaṁ;",
  "tha-ap549:3.3_0": "Samāpattī tathā aṭṭha,",
  "tha-ap549:3.4_0": "tisso vijjā pavecchati.",
  "tha-ap549:4.1_0": "Chaḷabhiññāsu yojesi,",
  "tha-ap549:4.2_0": "kiñci sattaṁ naruttamo;",
  "tha-ap549:4.3_0": "Deti kassaci nātho so,",
  "tha-ap549:4.4_0": "catasso paṭisambhidā.",
  "tha-ap549:5.1_0": "Bodhaneyyaṁ pajaṁ disvā,",
  "tha-ap549:5.2_0": "asaṅkheyyampi yojanaṁ;",
  "tha-ap549:5.3_0": "Khaṇena upagantvāna,",
  "tha-ap549:5.4_0": "vineti narasārathi.",
  "tha-ap549:6.1_0": "Tadāhaṁ haṁsavatiyaṁ,",
  "tha-ap549:6.2_0": "ahosiṁ brāhmaṇatrajo;",
  "tha-ap549:6.3_0": "Pāragū sabbavedānaṁ,",
  "tha-ap549:6.4_0": "veyyākaraṇasammato.",
  "tha-ap549:7.1_0": "Niruttiyā ca kusalo,",
  "tha-ap549:7.2_0": "nighaṇḍumhi visārado;",
  "tha-ap549:7.3_0": "Padako keṭubhavidū,",
  "tha-ap549:7.4_0": "chandovicitikovido.",
  "tha-ap549:8.1_0": "Jaṅghāvihāraṁ vicaraṁ,",
  "tha-ap549:8.2_0": "haṁsārāmamupeccahaṁ;",
  "tha-ap549:8.3_0": "Addasaṁ varadaṁ seṭṭhaṁ,",
  "tha-ap549:8.4_0": "mahājanapurakkhataṁ.",
  "tha-ap549:9.1_0": "Desentaṁ virajaṁ dhammaṁ,",
  "tha-ap549:9.2_0": "paccanīkamatī ahaṁ;",
  "tha-ap549:9.3_0": "Upetvā tassa kalyāṇaṁ,",
  "tha-ap549:9.4_0": "sutvāna vimalaṁ ahaṁ.",
  "tha-ap549:10.1_0": "Byāhataṁ punaruttaṁ vā,",
  "tha-ap549:10.2_0": "apatthaṁ vā niratthakaṁ;",
  "tha-ap549:10.3_0": "Nāddasaṁ tassa munino,",
  "tha-ap549:10.4_0": "tato pabbajito ahaṁ.",
  "tha-ap549:11.1_0": "Nacireneva kālena,",
  "tha-ap549:11.2_0": "sabbasattavisārado;",
  "tha-ap549:11.3_0": "Nipuṇo buddhavacane,",
  "tha-ap549:11.4_0": "ahosiṁ guṇisammato.",
  "tha-ap549:12.1_0": "Tadā catasso gāthāyo,",
  "tha-ap549:12.2_0": "ganthayitvā subyañjanā;",
  "tha-ap549:12.3_0": "Santhavitvā tilokaggaṁ,",
  "tha-ap549:12.4_0": "desayissaṁ dine dine.",
  "tha-ap549:13.1_0": "‘Virattosi mahāvīro,",
  "tha-ap549:13.2_0": "saṁsāre sabhaye vasaṁ;",
  "tha-ap549:13.3_0": "Karuṇāya na nibbāyi,",
  "tha-ap549:13.4_0": "tato kāruṇiko muni.",
  "tha-ap549:14.1_0": "Puthujjano vayo santo,",
  "tha-ap549:14.2_0": "na kilesavaso ahu;",
  "tha-ap549:14.3_0": "Sampajāno satiyutto,",
  "tha-ap549:14.4_0": "tasmā eso acintiyo.",
  "tha-ap549:15.1_0": "Dubbalāni kilesāni,",
  "tha-ap549:15.2_0": "yassāsayagatāni me;",
  "tha-ap549:15.3_0": "Ñāṇaggiparidaḍḍhāni,",
  "tha-ap549:15.4_0": "na khīyiṁsu tamabbhutaṁ.",
  "tha-ap549:16.1_0": "Yo sabbalokassa garu,",
  "tha-ap549:16.2_0": "loko yassa tathā garu;",
  "tha-ap549:16.3_0": "Tathāpi lokācariyo,",
  "tha-ap549:16.4_0": "loko tassānuvattako’.",
  "tha-ap549:17.1_0": "Evamādīhi sambuddhaṁ,",
  "tha-ap549:17.2_0": "kittayaṁ dhammadesanaṁ;",
  "tha-ap549:17.3_0": "Yāvajīvaṁ karitvāna,",
  "tha-ap549:17.4_0": "gato saggaṁ tato cuto.",
  "tha-ap549:18.1_0": "Satasahassito kappe,",
  "tha-ap549:18.2_0": "yaṁ buddhamabhikittayiṁ;",
  "tha-ap549:18.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap549:18.4_0": "kittanāya idaṁ phalaṁ.",
  "tha-ap549:19.1_0": "Devaloke mahārajjaṁ,",
  "tha-ap549:19.2_0": "pādesiṁ kañcanagghiyaṁ;",
  "tha-ap549:19.3_0": "Cakkavattī mahārajjaṁ,",
  "tha-ap549:19.4_0": "bahusonubhaviṁ ahaṁ.",
  "tha-ap549:20.1_0": "Duve bhave pajāyāmi,",
  "tha-ap549:20.2_0": "devatte atha mānuse;",
  "tha-ap549:20.3_0": "Aññaṁ gatiṁ na jānāmi,",
  "tha-ap549:20.4_0": "kittanāya idaṁ phalaṁ.",
  "tha-ap549:21.1_0": "Duve kule pajāyāmi,",
  "tha-ap549:21.2_0": "khattiye atha brāhmaṇe;",
  "tha-ap549:21.3_0": "Nīce kule na jāyāmi,",
  "tha-ap549:21.4_0": "kittanāya idaṁ phalaṁ.",
  "tha-ap549:22.1_0": "Pacchime ca bhave dāni,",
  "tha-ap549:22.2_0": "giribbajapuruttame;",
  "tha-ap549:22.3_0": "Raññohaṁ bimbisārassa,",
  "tha-ap549:22.4_0": "putto nāmena cābhayo.",
  "tha-ap549:23.1_0": "Pāpamittavasaṁ gantvā,",
  "tha-ap549:23.2_0": "nigaṇṭhena vimohito;",
  "tha-ap549:23.3_0": "Pesito nāṭaputtena,",
  "tha-ap549:23.4_0": "buddhaseṭṭhamupeccahaṁ.",
  "tha-ap549:24.1_0": "Pucchitvā nipuṇaṁ pañhaṁ,",
  "tha-ap549:24.2_0": "sutvā byākaraṇuttamaṁ;",
  "tha-ap549:24.3_0": "Pabbajitvāna naciraṁ,",
  "tha-ap549:24.4_0": "arahattamapāpuṇiṁ.",
  "tha-ap549:25.1_0": "Kittayitvā jinavaraṁ,",
  "tha-ap549:25.2_0": "kittito homi sabbadā;",
  "tha-ap549:25.3_0": "Sugandhadehavadano,",
  "tha-ap549:25.4_0": "āsiṁ sukhasamappito.",
  "tha-ap549:26.1_0": "Tikkhahāsalahupañño,",
  "tha-ap549:26.2_0": "mahāpañño tathevahaṁ;",
  "tha-ap549:26.3_0": "Vicittapaṭibhāno ca,",
  "tha-ap549:26.4_0": "tassa kammassa vāhasā.",
  "tha-ap549:27.1_0": "Abhitthavitvā padumuttarāhaṁ,",
  "tha-ap549:27.2_0": "Pasannacitto asamaṁ sayambhuṁ;",
  "tha-ap549:27.3_0": "Na gacchi kappāni apāyabhūmiṁ,",
  "tha-ap549:27.4_0": "Sataṁsahassāni balena tassa.",
  "tha-ap549:28.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap549:28.2_0": "…pe…",
  "tha-ap549:28.3_0": "viharāmi anāsavo.",
  "tha-ap549:29.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap549:29.2_0": "…pe…",
  "tha-ap549:29.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap549:30.1_0": "Paṭisambhidā catasso,",
  "tha-ap549:30.2_0": "…pe…",
  "tha-ap549:30.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap549:31.1_0": "Itthaṁ sudaṁ āyasmā abhayo thero imā gāthāyo abhāsitthāti.",
  "tha-ap549:32.1_0": "Abhayattherassāpadānaṁ sattamaṁ."
}