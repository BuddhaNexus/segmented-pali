{
  "cp23:0.1_0": "Cariyāpiṭaka",
  "cp23:0.2_0": "Yudhañjayavagga",
  "cp23:0.3_0": "Nekkhammapāramī 3",
  "cp23:0.4_0": "Ayogharacariya",
  "cp23:1.1_0": "“Punāparaṁ yadā homi,",
  "cp23:1.2_0": "kāsirājassa atrajo;",
  "cp23:1.3_0": "Ayogharamhi saṁvaḍḍho,",
  "cp23:1.4_0": "nāmenāsi ayogharo.",
  "cp23:2.1_0": "‘Dukkhena jīvito laddho,",
  "cp23:2.2_0": "saṁpīḷe patiposito;",
  "cp23:2.3_0": "Ajjeva putta paṭipajja,",
  "cp23:2.4_0": "kevalaṁ vasudhaṁ imaṁ’.",
  "cp23:3.1_0": "Saraṭṭhakaṁ sanigamaṁ,",
  "cp23:3.2_0": "sajanaṁ vanditva khattiyaṁ;",
  "cp23:3.3_0": "Añjaliṁ paggahetvāna,",
  "cp23:3.4_0": "idaṁ vacanamabraviṁ.",
  "cp23:4.1_0": "‘Ye keci mahiyā sattā,",
  "cp23:4.2_0": "hīnamukkaṭṭhamajjhimā;",
  "cp23:4.3_0": "Nirārakkhā sake gehe,",
  "cp23:4.4_0": "vaḍḍhanti sakañātibhi.",
  "cp23:5.1_0": "Idaṁ loke uttariyaṁ,",
  "cp23:5.2_0": "saṁpīḷe mama posanaṁ;",
  "cp23:5.3_0": "Ayogharamhi saṁvaḍḍho,",
  "cp23:5.4_0": "appabhe candasūriye.",
  "cp23:6.1_0": "Pūtikuṇapasampuṇṇā,",
  "cp23:6.2_0": "muccitvā mātukucchito;",
  "cp23:6.3_0": "Tato ghoratare dukkhe,",
  "cp23:6.4_0": "puna pakkhittayoghare.",
  "cp23:7.1_0": "Yadihaṁ tādisaṁ patvā,",
  "cp23:7.2_0": "dukkhaṁ paramadāruṇaṁ;",
  "cp23:7.3_0": "Rajjesu yadi rajjāmi,",
  "cp23:7.4_0": "pāpānaṁ uttamo siyaṁ.",
  "cp23:8.1_0": "Ukkaṇṭhitomhi kāyena,",
  "cp23:8.2_0": "rajjenamhi anatthiko;",
  "cp23:8.3_0": "Nibbutiṁ pariyesissaṁ,",
  "cp23:8.4_0": "yattha maṁ maccu na maddiye’.",
  "cp23:9.1_0": "Evāhaṁ cintayitvāna,",
  "cp23:9.2_0": "viravante mahājane;",
  "cp23:9.3_0": "Nāgova bandhanaṁ chetvā,",
  "cp23:9.4_0": "pāvisiṁ kānanaṁ vanaṁ.",
  "cp23:10.1_0": "Mātāpitā na me dessā,",
  "cp23:10.2_0": "napi me dessaṁ mahāyasaṁ;",
  "cp23:10.3_0": "Sabbaññutaṁ piyaṁ mayhaṁ,",
  "cp23:10.4_0": "tasmā rajjaṁ pariccajin”ti.",
  "cp23:11.1_0": "Ayogharacariyaṁ tatiyaṁ."
}