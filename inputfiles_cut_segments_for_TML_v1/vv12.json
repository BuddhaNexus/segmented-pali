{
  "vv12:0.1_0": "Vimānavatthu",
  "vv12:0.2_0": "Itthivimāna",
  "vv12:0.3_0": "Pīṭhavagga",
  "vv12:0.4_0": "12. Dutiyapatibbatāvimānavatthu",
  "vv12:1.1_0": "“Veḷuriyathambhaṁ ruciraṁ pabhassaraṁ,",
  "vv12:1.2_0": "Vimānamāruyha anekacittaṁ;",
  "vv12:1.3_0": "Tatthacchasi devi mahānubhāve,",
  "vv12:1.4_0": "Uccāvacā iddhi vikubbamānā;",
  "vv12:1.5_0": "Imā ca te accharāyo samantato,",
  "vv12:1.6_0": "Naccanti gāyanti pamodayanti ca.",
  "vv12:2.1_0": "Deviddhipattāsi mahānubhāve,",
  "vv12:2.2_0": "Manussabhūtā kimakāsi puññaṁ;",
  "vv12:2.3_0": "Kenāsi evaṁ jalitānubhāvā,",
  "vv12:2.4_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv12:3.1_0": "Sā devatā attamanā,",
  "vv12:3.2_0": "moggallānena pucchitā;",
  "vv12:3.3_0": "Pañhaṁ puṭṭhā viyākāsi,",
  "vv12:3.4_0": "yassa kammassidaṁ phalaṁ.",
  "vv12:4.1_0": "“Ahaṁ manussesu manussabhūtā,",
  "vv12:4.2_0": "Upāsikā cakkhumato ahosiṁ;",
  "vv12:4.3_0": "Pāṇātipātā viratā ahosiṁ,",
  "vv12:4.4_0": "Loke adinnaṁ parivajjayissaṁ.",
  "vv12:5.1_0": "Amajjapā no ca musā abhāṇiṁ,",
  "vv12:5.2_0": "Sakena sāminā ahosiṁ tuṭṭhā;",
  "vv12:5.3_0": "Annañca pānañca pasannacittā,",
  "vv12:5.4_0": "Sakkacca dānaṁ vipulaṁ adāsiṁ.",
  "vv12:6.1_0": "Tena metādiso vaṇṇo,",
  "vv12:6.2_0": "tena me idha mijjhati;",
  "vv12:6.3_0": "Uppajjanti ca me bhogā,",
  "vv12:6.4_0": "ye keci manaso piyā.",
  "vv12:7.1_0": "Akkhāmi te bhikkhu mahānubhāva,",
  "vv12:7.2_0": "Manussabhūtā yamakāsi puññaṁ;",
  "vv12:7.3_0": "Tenamhi evaṁ jalitānubhāvā,",
  "vv12:7.4_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv12:8.1_0": "Dutiyapatibbatāvimānaṁ dvādasamaṁ."
}