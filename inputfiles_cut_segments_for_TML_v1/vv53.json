{
  "vv53:0.1_0": "Vimānavatthu",
  "vv53:0.2_0": "Purisavimāna",
  "vv53:0.3_0": "Mahārathavagga",
  "vv53:0.4_0": "3. Chattamāṇavakavimānavatthu",
  "vv53:1.1_0": "“Yo vadataṁ pavaro manujesu,",
  "vv53:1.2_0": "Sakyamunī bhagavā katakicco;",
  "vv53:1.3_0": "Pāragato balaviriyasamaṅgī,",
  "vv53:1.4_0": "Taṁ sugataṁ saraṇatthamupehi.",
  "vv53:2.1_0": "Rāgavirāgamanejamasokaṁ,",
  "vv53:2.2_0": "Dhammamasaṅkhatamappaṭikūlaṁ;",
  "vv53:2.3_0": "Madhuramimaṁ paguṇaṁ suvibhattaṁ,",
  "vv53:2.4_0": "Dhammamimaṁ saraṇatthamupehi.",
  "vv53:3.1_0": "Yattha ca dinna mahapphalamāhu,",
  "vv53:3.2_0": "Catūsu sucīsu purisayugesu;",
  "vv53:3.3_0": "Aṭṭha ca puggaladhammadasā te,",
  "vv53:3.4_0": "Saṅghamimaṁ saraṇatthamupehi.",
  "vv53:4.1_0": "Na tathā tapati nabhe sūriyo,",
  "vv53:4.2_0": "Cando ca na bhāsati na phusso;",
  "vv53:4.3_0": "Yathā atulamidaṁ mahappabhāsaṁ,",
  "vv53:4.4_0": "Ko nu tvaṁ tidivā mahiṁ upāgā.",
  "vv53:5.1_0": "Chindati raṁsī pabhaṅkarassa,",
  "vv53:5.2_0": "Sādhikavīsatiyojanāni ābhā;",
  "vv53:5.3_0": "Rattimapi yathā divaṁ karoti,",
  "vv53:5.4_0": "Parisuddhaṁ vimalaṁ subhaṁ vimānaṁ.",
  "vv53:6.1_0": "Bahupadumavicitrapuṇḍarīkaṁ,",
  "vv53:6.2_0": "Vokiṇṇaṁ kusumehi nekacittaṁ;",
  "vv53:6.3_0": "Arajavirajahemajālachannaṁ,",
  "vv53:6.4_0": "Ākāse tapati yathāpi sūriyo.",
  "vv53:7.1_0": "Rattambarapītavāsasāhi,",
  "vv53:7.2_0": "Agarupiyaṅgucandanussadāhi;",
  "vv53:7.3_0": "Kañcanatanusannibhattacāhi,",
  "vv53:7.4_0": "Paripūraṁ gaganaṁva tārakāhi.",
  "vv53:8.1_0": "Naranāriyo bahuketthanekavaṇṇā,",
  "vv53:8.2_0": "Kusumavibhūsitābharaṇettha sumanā;",
  "vv53:8.3_0": "Anilapamuñcitā pavanti surabhiṁ,",
  "vv53:8.4_0": "Tapaniyavitatā suvaṇṇachannā.",
  "vv53:9.1_0": "Kissa saṁyamassa ayaṁ vipāko,",
  "vv53:9.2_0": "Kenāsi kammaphalenidhūpapanno;",
  "vv53:9.3_0": "Yathā ca te adhigatamidaṁ vimānaṁ,",
  "vv53:9.4_0": "Tadanupadaṁ avacāsi iṅgha puṭṭho”ti.",
  "vv53:10.1_0": "“Sayamidha pathe samecca māṇavena,",
  "vv53:10.2_0": "Satthānusāsi anukampamāno;",
  "vv53:10.3_0": "Tava ratanavarassa dhammaṁ sutvā,",
  "vv53:10.4_0": "Karissāmīti ca bravittha chatto.",
  "vv53:11.1_0": "Jinavarapavaraṁ upehi saraṇaṁ,",
  "vv53:11.2_0": "Dhammañcāpi tatheva bhikkhusaṅghaṁ;",
  "vv53:11.3_0": "Noti paṭhamaṁ avocahaṁ bhante,",
  "vv53:11.4_0": "Pacchā te vacanaṁ tathevakāsiṁ.",
  "vv53:12.1_0": "Mā ca pāṇavadhaṁ vividhaṁ carassu asuciṁ,",
  "vv53:12.2_0": "Na hi pāṇesu asaññataṁ avaṇṇayiṁsu sappaññā;",
  "vv53:12.3_0": "Noti paṭhamaṁ avocahaṁ bhante,",
  "vv53:12.4_0": "Pacchā te vacanaṁ tathevakāsiṁ.",
  "vv53:13.1_0": "Mā ca parajanassa rakkhitampi,",
  "vv53:13.2_0": "Ādātabbamamaññitho adinnaṁ;",
  "vv53:13.3_0": "Noti paṭhamaṁ avocahaṁ bhante,",
  "vv53:13.4_0": "Pacchā te vacanaṁ tathevakāsiṁ.",
  "vv53:14.1_0": "Mā ca parajanassa rakkhitāyo,",
  "vv53:14.2_0": "Parabhariyā agamā anariyametaṁ;",
  "vv53:14.3_0": "Noti paṭhamaṁ avocahaṁ bhante,",
  "vv53:14.4_0": "Pacchā te vacanaṁ tathevakāsiṁ.",
  "vv53:15.1_0": "Mā ca vitathaṁ aññathā abhāṇi,",
  "vv53:15.2_0": "Na hi musāvādaṁ avaṇṇayiṁsu sappaññā;",
  "vv53:15.3_0": "Noti paṭhamaṁ avocahaṁ bhante,",
  "vv53:15.4_0": "Pacchā te vacanaṁ tathevakāsiṁ.",
  "vv53:16.1_0": "Yena ca purisassa apeti saññā,",
  "vv53:16.2_0": "Taṁ majjaṁ parivajjayassu sabbaṁ;",
  "vv53:16.3_0": "Noti paṭhamaṁ avocahaṁ bhante,",
  "vv53:16.4_0": "Pacchā te vacanaṁ tathevakāsiṁ.",
  "vv53:17.1_0": "Svāhaṁ idha pañca sikkhā karitvā,",
  "vv53:17.2_0": "Paṭipajjitvā tathāgatassa dhamme;",
  "vv53:17.3_0": "Dvepathamagamāsiṁ coramajjhe,",
  "vv53:17.4_0": "Te maṁ tattha vadhiṁsu bhogahetu.",
  "vv53:18.1_0": "Ettakamidaṁ anussarāmi kusalaṁ,",
  "vv53:18.2_0": "Tato paraṁ na me vijjati aññaṁ;",
  "vv53:18.3_0": "Tena sucaritena kammunāhaṁ,",
  "vv53:18.4_0": "Uppanno tidivesu kāmakāmī.",
  "vv53:19.1_0": "Passa khaṇamuhuttasaññamassa,",
  "vv53:19.2_0": "Anudhammappaṭipattiyā vipākaṁ;",
  "vv53:19.3_0": "Jalamiva yasasā samekkhamānā,",
  "vv53:19.4_0": "Bahukā maṁ pihayanti hīnakammā.",
  "vv53:20.1_0": "Passa katipayāya desanāya,",
  "vv53:20.2_0": "Sugatiñcamhi gato sukhañca patto;",
  "vv53:20.3_0": "Ye ca te satataṁ suṇanti dhammaṁ,",
  "vv53:20.4_0": "Maññe te amataṁ phusanti khemaṁ.",
  "vv53:21.1_0": "Appampi kataṁ mahāvipākaṁ,",
  "vv53:21.2_0": "Vipulaṁ hoti tathāgatassa dhamme;",
  "vv53:21.3_0": "Passa katapuññatāya chatto,",
  "vv53:21.4_0": "Obhāseti pathaviṁ yathāpi sūriyo.",
  "vv53:22.1_0": "Kimidaṁ kusalaṁ kimācarema,",
  "vv53:22.2_0": "Icceke hi samecca mantayanti;",
  "vv53:22.3_0": "Te mayaṁ punareva laddha mānusattaṁ,",
  "vv53:22.4_0": "Paṭipannā viharemu sīlavanto.",
  "vv53:23.1_0": "Bahukāro anukampako ca satthā,",
  "vv53:23.2_0": "Iti me sati agamā divā divassa;",
  "vv53:23.3_0": "Svāhaṁ upagatomhi saccanāmaṁ,",
  "vv53:23.4_0": "Anukampassu punapi suṇemu dhammaṁ.",
  "vv53:24.1_0": "Ye cidha pajahanti kāmarāgaṁ,",
  "vv53:24.2_0": "Bhavarāgānusayañca pahāya mohaṁ;",
  "vv53:24.3_0": "Na ca te punamupenti gabbhaseyyaṁ,",
  "vv53:24.4_0": "Parinibbānagatā hi sītibhūtā”ti.",
  "vv53:25.1_0": "Chattamāṇavakavimānaṁ tatiyaṁ."
}