{
  "ja381:0.1_0": "Jātaka",
  "ja381:0.2_0": "Chakkanipāta",
  "ja381:0.3_0": "Avāriyavagga",
  "ja381:0.4_0": "6. Migālopajātaka",
  "ja381:1.1_0": "“Na me rucci migālopa,",
  "ja381:1.2_0": "yassa te tādisī gatī;",
  "ja381:1.3_0": "Atuccaṁ tāta patasi,",
  "ja381:1.4_0": "abhūmiṁ tāta sevasi.",
  "ja381:2.1_0": "Catukkaṇṇaṁva kedāraṁ,",
  "ja381:2.2_0": "yadā te pathavī siyā;",
  "ja381:2.3_0": "Tato tāta nivattassu,",
  "ja381:2.4_0": "māssu etto paraṁ gami”.",
  "ja381:3.1_0": "“Santi aññepi sakuṇā,",
  "ja381:3.2_0": "pattayānā vihaṅgamā;",
  "ja381:3.3_0": "Akkhittā vātavegena,",
  "ja381:3.4_0": "naṭṭhā te sassatīsamā.",
  "ja381:4.1_0": "Akatvā apanandassa,",
  "ja381:4.2_0": "pitu vuddhassa sāsanaṁ;",
  "ja381:4.3_0": "Kālavāte atikkamma,",
  "ja381:4.4_0": "verambhānaṁ vasaṁ agā.",
  "ja381:5.1_0": "Tassa puttā ca dārā ca,",
  "ja381:5.2_0": "ye caññe anujīvino;",
  "ja381:5.3_0": "Sabbe byasanamāpāduṁ,",
  "ja381:5.4_0": "anovādakare dije.",
  "ja381:6.1_0": "Evampi idha vuddhānaṁ,",
  "ja381:6.2_0": "yo vākyaṁ nāvabujjhati;",
  "ja381:6.3_0": "Atisīmacaro ditto,",
  "ja381:6.4_0": "gijjhovātītasāsano;",
  "ja381:6.5_0": "Sabbe byasanaṁ papponti,",
  "ja381:6.6_0": "akatvā vuddhasāsanan”ti.",
  "ja381:7.1_0": "Migālopajātakaṁ chaṭṭhaṁ."
}