{
  "thag16.6:0.1_0": "Theragāthā",
  "thag16.6:0.2_0": "Vīsatinipāta",
  "thag16.6:0.3_0": "Paṭhamavagga",
  "thag16.6:0.4_0": "16.6. Selattheragāthā",
  "thag16.6:1.1_0": "“Paripuṇṇakāyo suruci,",
  "thag16.6:1.2_0": "Sujāto cārudassano;",
  "thag16.6:1.3_0": "Suvaṇṇavaṇṇosi bhagavā,",
  "thag16.6:1.4_0": "Susukkadāṭhosi vīriyavā.",
  "thag16.6:2.1_0": "Narassa hi sujātassa,",
  "thag16.6:2.2_0": "ye bhavanti viyañjanā;",
  "thag16.6:2.3_0": "Sabbe te tava kāyasmiṁ,",
  "thag16.6:2.4_0": "mahāpurisalakkhaṇā.",
  "thag16.6:3.1_0": "Pasannanetto sumukho,",
  "thag16.6:3.2_0": "brahā uju patāpavā;",
  "thag16.6:3.3_0": "Majjhe samaṇasaṅghassa,",
  "thag16.6:3.4_0": "ādiccova virocasi.",
  "thag16.6:4.1_0": "Kalyāṇadassano bhikkhu,",
  "thag16.6:4.2_0": "kañcanasannibhattaco;",
  "thag16.6:4.3_0": "Kiṁ te samaṇabhāvena,",
  "thag16.6:4.4_0": "evaṁ uttamavaṇṇino.",
  "thag16.6:5.1_0": "Rājā arahasi bhavituṁ,",
  "thag16.6:5.2_0": "cakkavattī rathesabho;",
  "thag16.6:5.3_0": "Cāturanto vijitāvī,",
  "thag16.6:5.4_0": "jambusaṇḍassa issaro.",
  "thag16.6:6.1_0": "Khattiyā bhogī rājāno,",
  "thag16.6:6.2_0": "anuyantā bhavanti te;",
  "thag16.6:6.3_0": "Rājābhirājā manujindo,",
  "thag16.6:6.4_0": "rajjaṁ kārehi gotama”.",
  "thag16.6:7.1_0": "“Rājāhamasmi sela,",
  "thag16.6:7.2_0": "(selāti bhagavā)",
  "thag16.6:7.3_0": "Dhammarājā anuttaro;",
  "thag16.6:7.4_0": "Dhammena cakkaṁ vattemi,",
  "thag16.6:7.5_0": "Cakkaṁ appaṭivattiyaṁ”.",
  "thag16.6:8.1_0": "“Sambuddho paṭijānāsi,",
  "thag16.6:8.2_0": "(iti selo brāhmaṇo)",
  "thag16.6:8.3_0": "Dhammarājā anuttaro;",
  "thag16.6:8.4_0": "‘Dhammena cakkaṁ vattemi’,",
  "thag16.6:8.5_0": "Iti bhāsatha gotama.",
  "thag16.6:9.1_0": "Ko nu senāpati bhoto,",
  "thag16.6:9.2_0": "sāvako satthuranvayo;",
  "thag16.6:9.3_0": "Ko tetamanuvatteti,",
  "thag16.6:9.4_0": "dhammacakkaṁ pavattitaṁ”.",
  "thag16.6:10.1_0": "“Mayā pavattitaṁ cakkaṁ,",
  "thag16.6:10.2_0": "(selāti bhagavā)",
  "thag16.6:10.3_0": "Dhammacakkaṁ anuttaraṁ;",
  "thag16.6:10.4_0": "Sāriputto anuvatteti,",
  "thag16.6:10.5_0": "Anujāto tathāgataṁ.",
  "thag16.6:11.1_0": "Abhiññeyyaṁ abhiññātaṁ,",
  "thag16.6:11.2_0": "bhāvetabbañca bhāvitaṁ;",
  "thag16.6:11.3_0": "Pahātabbaṁ pahīnaṁ me,",
  "thag16.6:11.4_0": "tasmā buddhosmi brāhmaṇa.",
  "thag16.6:12.1_0": "Vinayassu mayi kaṅkhaṁ,",
  "thag16.6:12.2_0": "adhimuccassu brāhmaṇa;",
  "thag16.6:12.3_0": "Dullabhaṁ dassanaṁ hoti,",
  "thag16.6:12.4_0": "sambuddhānaṁ abhiṇhaso.",
  "thag16.6:13.1_0": "Yesaṁ ve dullabho loke,",
  "thag16.6:13.2_0": "pātubhāvo abhiṇhaso;",
  "thag16.6:13.3_0": "Sohaṁ brāhmaṇa buddhosmi,",
  "thag16.6:13.4_0": "sallakatto anuttaro.",
  "thag16.6:14.1_0": "Brahmabhūto atitulo,",
  "thag16.6:14.2_0": "mārasenappamaddano;",
  "thag16.6:14.3_0": "Sabbāmitte vase katvā,",
  "thag16.6:14.4_0": "modāmi akutobhayo”.",
  "thag16.6:15.1_0": "“Idaṁ bhonto nisāmetha,",
  "thag16.6:15.2_0": "yathā bhāsati cakkhumā;",
  "thag16.6:15.3_0": "Sallakatto mahāvīro,",
  "thag16.6:15.4_0": "sīhova nadatī vane.",
  "thag16.6:16.1_0": "Brahmabhūtaṁ atitulaṁ,",
  "thag16.6:16.2_0": "mārasenappamaddanaṁ;",
  "thag16.6:16.3_0": "Ko disvā nappasīdeyya,",
  "thag16.6:16.4_0": "api kaṇhābhijātiko.",
  "thag16.6:17.1_0": "Yo maṁ icchati anvetu,",
  "thag16.6:17.2_0": "yo vā nicchati gacchatu;",
  "thag16.6:17.3_0": "Idhāhaṁ pabbajissāmi,",
  "thag16.6:17.4_0": "varapaññassa santike”.",
  "thag16.6:18.1_0": "“Etañce ruccati bhoto,",
  "thag16.6:18.2_0": "sammāsambuddhasāsanaṁ;",
  "thag16.6:18.3_0": "Mayampi pabbajissāma,",
  "thag16.6:18.4_0": "varapaññassa santike”.",
  "thag16.6:19.1_0": "“Brāhmaṇā tisatā ime,",
  "thag16.6:19.2_0": "yācanti pañjalīkatā;",
  "thag16.6:19.3_0": "‘Brahmacariyaṁ carissāma,",
  "thag16.6:19.4_0": "bhagavā tava santike’”.",
  "thag16.6:20.1_0": "“Svākkhātaṁ brahmacariyaṁ,",
  "thag16.6:20.2_0": "(selāti bhagavā)",
  "thag16.6:20.3_0": "Sandiṭṭhikamakālikaṁ;",
  "thag16.6:20.4_0": "Yattha amoghā pabbajjā,",
  "thag16.6:20.5_0": "Appamattassa sikkhato”.",
  "thag16.6:21.1_0": "“Yaṁ taṁ saraṇamāgamha,",
  "thag16.6:21.2_0": "ito aṭṭhame cakkhuma;",
  "thag16.6:21.3_0": "Sattarattena bhagavā,",
  "thag16.6:21.4_0": "dantāmha tava sāsane.",
  "thag16.6:22.1_0": "Tuvaṁ buddho tuvaṁ satthā,",
  "thag16.6:22.2_0": "tuvaṁ mārābhibhū muni;",
  "thag16.6:22.3_0": "Tuvaṁ anusaye chetvā,",
  "thag16.6:22.4_0": "tiṇṇo tāresimaṁ pajaṁ.",
  "thag16.6:23.1_0": "Upadhī te samatikkantā,",
  "thag16.6:23.2_0": "āsavā te padālitā;",
  "thag16.6:23.3_0": "Sīhova anupādāno,",
  "thag16.6:23.4_0": "pahīnabhayabheravo.",
  "thag16.6:24.1_0": "Bhikkhavo tisatā ime,",
  "thag16.6:24.2_0": "tiṭṭhanti pañjalīkatā;",
  "thag16.6:24.3_0": "Pāde vīra pasārehi,",
  "thag16.6:24.4_0": "nāgā vandantu satthuno”ti.",
  "thag16.6:24.5_0": "… Selo thero …."
}