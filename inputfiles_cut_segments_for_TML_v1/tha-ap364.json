{
  "tha-ap364:0.1_0": "Therāpadāna",
  "tha-ap364:0.2_0": "Mandāravapupphiyavagga",
  "tha-ap364:0.3_0": "2. Kakkārupupphiyattheraapadāna",
  "tha-ap364:1.1_0": "“Yāmā devā idhāgantvā,",
  "tha-ap364:1.2_0": "gotamaṁ sirivacchasaṁ;",
  "tha-ap364:1.3_0": "Kakkārumālaṁ paggayha,",
  "tha-ap364:1.4_0": "buddhassa abhiropayiṁ.",
  "tha-ap364:2.1_0": "Dvenavute ito kappe,",
  "tha-ap364:2.2_0": "yaṁ buddhamabhipūjayiṁ;",
  "tha-ap364:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap364:2.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap364:3.1_0": "Paṭisambhidā catasso,",
  "tha-ap364:3.2_0": "…pe…",
  "tha-ap364:3.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap364:4.1_0": "Itthaṁ sudaṁ āyasmā kakkārupupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap364:5.1_0": "Kakkārupupphiyattherassāpadānaṁ dutiyaṁ."
}