{
  "ja466:0.1_0": "Jātaka",
  "ja466:0.2_0": "Dvādasakanipāta",
  "ja466:0.3_0": "Cūḷakuṇālavagga",
  "ja466:0.4_0": "3. Samuddavāṇijajātaka",
  "ja466:1.1_0": "“Kasanti vapanti te janā,",
  "ja466:1.2_0": "Manujā kammaphalūpajīvino;",
  "ja466:1.3_0": "Nayimassa dīpakassa bhāgino,",
  "ja466:1.4_0": "Jambudīpā idameva no varaṁ”.",
  "ja466:2.1_0": "“Tipañcarattūpagatamhi cande,",
  "ja466:2.2_0": "Vego mahā hehiti sāgarassa;",
  "ja466:2.3_0": "Uplavissaṁ dīpamimaṁ uḷāraṁ,",
  "ja466:2.4_0": "Mā vo vadhī gacchatha leṇamaññaṁ”.",
  "ja466:3.1_0": "“Na jātuyaṁ sāgaravārivego,",
  "ja466:3.2_0": "Uplavissaṁ dīpamimaṁ uḷāraṁ;",
  "ja466:3.3_0": "Taṁ me nimittehi bahūhi diṭṭhaṁ,",
  "ja466:3.4_0": "Mā bhetha kiṁ socatha modathavho.",
  "ja466:4.1_0": "Pahūtabhakkhaṁ bahuannapānaṁ,",
  "ja466:4.2_0": "Pattattha āvāsamimaṁ uḷāraṁ;",
  "ja466:4.3_0": "Na vo bhayaṁ paṭipassāmi kiñci,",
  "ja466:4.4_0": "Āputtaputtehi pamodathavho”.",
  "ja466:5.1_0": "“Yo devoyaṁ dakkhiṇāyaṁ disāyaṁ,",
  "ja466:5.2_0": "Khemanti pakkosati tassa saccaṁ;",
  "ja466:5.3_0": "Na uttaro vedi bhayābhayassa,",
  "ja466:5.4_0": "Mā bhetha kiṁ socatha modathavho”.",
  "ja466:6.1_0": "“Yathā ime vippavadanti yakkhā,",
  "ja466:6.2_0": "Eko bhayaṁ saṁsati khemameko;",
  "ja466:6.3_0": "Tadiṅgha mayhaṁ vacanaṁ suṇātha,",
  "ja466:6.4_0": "Khippaṁ lahuṁ mā vinassimha sabbe.",
  "ja466:7.1_0": "Sabbe samāgamma karoma nāvaṁ,",
  "ja466:7.2_0": "Doṇiṁ daḷhaṁ sabbayantūpapannaṁ;",
  "ja466:7.3_0": "Sace ayaṁ dakkhiṇo saccamāha,",
  "ja466:7.4_0": "Moghaṁ paṭikkosati uttaroyaṁ;",
  "ja466:7.5_0": "Sā ceva no hehiti āpadatthā,",
  "ja466:7.6_0": "Imañca dīpaṁ na pariccajema.",
  "ja466:8.1_0": "Sace ca kho uttaro saccamāha,",
  "ja466:8.2_0": "Moghaṁ paṭikkosati dakkhiṇoyaṁ;",
  "ja466:8.3_0": "Tameva nāvaṁ abhiruyha sabbe,",
  "ja466:8.4_0": "Evaṁ mayaṁ sotthi taremu pāraṁ.",
  "ja466:9.1_0": "Na ve sugaṇhaṁ paṭhamena seṭṭhaṁ,",
  "ja466:9.2_0": "Kaniṭṭhamāpāthagataṁ gahetvā;",
  "ja466:9.3_0": "Yo cīdha tacchaṁ paviceyya gaṇhati,",
  "ja466:9.4_0": "Sa ve naro seṭṭhamupeti ṭhānaṁ.",
  "ja466:10.1_0": "Yathāpi te sāgaravārimajjhe,",
  "ja466:10.2_0": "Sakammunā sotthi vahiṁsu vāṇijā;",
  "ja466:10.3_0": "Anāgatatthaṁ paṭivijjhiyāna,",
  "ja466:10.4_0": "Appampi nācceti sa bhūripañño.",
  "ja466:11.1_0": "Bālā ca mohena rasānugiddhā,",
  "ja466:11.2_0": "Anāgataṁ appaṭivijjhiyatthaṁ;",
  "ja466:11.3_0": "Paccuppanne sīdanti atthajāte,",
  "ja466:11.4_0": "Samuddamajjhe yathā te manussā.",
  "ja466:12.1_0": "Anāgataṁ paṭikayirātha kiccaṁ,",
  "ja466:12.2_0": "‘Mā maṁ kiccaṁ kiccakāle byadhesi’;",
  "ja466:12.3_0": "Taṁ tādisaṁ paṭikatakiccakāriṁ,",
  "ja466:12.4_0": "Na taṁ kiccaṁ kiccakāle byadhetī”ti.",
  "ja466:13.1_0": "Samuddavāṇijajātakaṁ tatiyaṁ."
}