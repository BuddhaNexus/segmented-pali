{
  "tha-ap179:0.1_0": "Therāpadāna",
  "tha-ap179:0.2_0": "Kumudavagga",
  "tha-ap179:0.3_0": "7. Ekadīpiyattheraapadāna",
  "tha-ap179:1.1_0": "“Padumuttarassa munino,",
  "tha-ap179:1.2_0": "saḷale bodhimuttame;",
  "tha-ap179:1.3_0": "Pasannacitto sumano,",
  "tha-ap179:1.4_0": "ekadīpaṁ adāsahaṁ.",
  "tha-ap179:2.1_0": "Bhave nibbattamānamhi,",
  "tha-ap179:2.2_0": "nibbatte puññasañcaye;",
  "tha-ap179:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap179:2.4_0": "dīpadānassidaṁ phalaṁ.",
  "tha-ap179:3.1_0": "Soḷase kappasahasse,",
  "tha-ap179:3.2_0": "ito te caturo janā;",
  "tha-ap179:3.3_0": "Candābhā nāma nāmena,",
  "tha-ap179:3.4_0": "cakkavattī mahabbalā.",
  "tha-ap179:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap179:4.2_0": "…pe…",
  "tha-ap179:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap179:5.1_0": "Itthaṁ sudaṁ āyasmā ekadīpiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap179:6.1_0": "Ekadīpiyattherassāpadānaṁ sattamaṁ.",
  "tha-ap179:6.2_0": "Navamaṁ bhāṇavāraṁ."
}