{
  "tha-ap127:0.1_0": "Therāpadāna",
  "tha-ap127:0.2_0": "Sereyyavagga",
  "tha-ap127:0.3_0": "5. Sammukhāthavikattheraapadāna",
  "tha-ap127:1.1_0": "“Jāyamāne vipassimhi,",
  "tha-ap127:1.2_0": "nimittaṁ byākariṁ ahaṁ;",
  "tha-ap127:1.3_0": "‘Nibbāpayiñca janataṁ,",
  "tha-ap127:1.4_0": "buddho loke bhavissati.",
  "tha-ap127:2.1_0": "Yasmiñca jāyamānasmiṁ,",
  "tha-ap127:2.2_0": "dasasahassi kampati;",
  "tha-ap127:2.3_0": "So dāni bhagavā satthā,",
  "tha-ap127:2.4_0": "dhammaṁ deseti cakkhumā.",
  "tha-ap127:3.1_0": "Yasmiñca jāyamānasmiṁ,",
  "tha-ap127:3.2_0": "āloko vipulo ahu;",
  "tha-ap127:3.3_0": "So dāni bhagavā satthā,",
  "tha-ap127:3.4_0": "dhammaṁ deseti cakkhumā.",
  "tha-ap127:4.1_0": "Yasmiñca jāyamānasmiṁ,",
  "tha-ap127:4.2_0": "saritāyo na sandayuṁ;",
  "tha-ap127:4.3_0": "So dāni bhagavā satthā,",
  "tha-ap127:4.4_0": "dhammaṁ deseti cakkhumā.",
  "tha-ap127:5.1_0": "Yasmiñca jāyamānasmiṁ,",
  "tha-ap127:5.2_0": "avīcaggi na pajjali;",
  "tha-ap127:5.3_0": "So dāni bhagavā satthā,",
  "tha-ap127:5.4_0": "dhammaṁ deseti cakkhumā.",
  "tha-ap127:6.1_0": "Yasmiñca jāyamānasmiṁ,",
  "tha-ap127:6.2_0": "pakkhisaṅgho na sañcari;",
  "tha-ap127:6.3_0": "So dāni bhagavā satthā,",
  "tha-ap127:6.4_0": "dhammaṁ deseti cakkhumā.",
  "tha-ap127:7.1_0": "Yasmiñca jāyamānasmiṁ,",
  "tha-ap127:7.2_0": "vātakkhandho na vāyati;",
  "tha-ap127:7.3_0": "So dāni bhagavā satthā,",
  "tha-ap127:7.4_0": "dhammaṁ deseti cakkhumā.",
  "tha-ap127:8.1_0": "Yasmiñca jāyamānasmiṁ,",
  "tha-ap127:8.2_0": "sabbaratanāni jotayuṁ;",
  "tha-ap127:8.3_0": "So dāni bhagavā satthā,",
  "tha-ap127:8.4_0": "dhammaṁ deseti cakkhumā.",
  "tha-ap127:9.1_0": "Yasmiñca jāyamānasmiṁ,",
  "tha-ap127:9.2_0": "sattāsuṁ padavikkamā;",
  "tha-ap127:9.3_0": "So dāni bhagavā satthā,",
  "tha-ap127:9.4_0": "dhammaṁ deseti cakkhumā.",
  "tha-ap127:10.1_0": "Jātamatto ca sambuddho,",
  "tha-ap127:10.2_0": "disā sabbā vilokayi;",
  "tha-ap127:10.3_0": "Vācāsabhimudīresi,",
  "tha-ap127:10.4_0": "esā buddhāna dhammatā’.",
  "tha-ap127:11.1_0": "Saṁvejayitvā janataṁ,",
  "tha-ap127:11.2_0": "thavitvā lokanāyakaṁ;",
  "tha-ap127:11.3_0": "Sambuddhaṁ abhivādetvā,",
  "tha-ap127:11.4_0": "pakkāmiṁ pācināmukho.",
  "tha-ap127:12.1_0": "Ekanavutito kappe,",
  "tha-ap127:12.2_0": "yaṁ buddhamabhithomayiṁ;",
  "tha-ap127:12.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap127:12.4_0": "thomanāya idaṁ phalaṁ.",
  "tha-ap127:13.1_0": "Ito navutikappamhi,",
  "tha-ap127:13.2_0": "sammukhāthavikavhayo;",
  "tha-ap127:13.3_0": "Sattaratanasampanno,",
  "tha-ap127:13.4_0": "cakkavattī mahabbalo.",
  "tha-ap127:14.1_0": "Pathavīdundubhi nāma,",
  "tha-ap127:14.2_0": "ekūnanavutimhito;",
  "tha-ap127:14.3_0": "Sattaratanasampanno,",
  "tha-ap127:14.4_0": "cakkavattī mahabbalo.",
  "tha-ap127:15.1_0": "Aṭṭhāsītimhito kappe,",
  "tha-ap127:15.2_0": "obhāso nāma khattiyo;",
  "tha-ap127:15.3_0": "Sattaratanasampanno,",
  "tha-ap127:15.4_0": "cakkavattī mahabbalo.",
  "tha-ap127:16.1_0": "Sattāsītimhito kappe,",
  "tha-ap127:16.2_0": "saritacchedanavhayo;",
  "tha-ap127:16.3_0": "Sattaratanasampanno,",
  "tha-ap127:16.4_0": "cakkavattī mahabbalo.",
  "tha-ap127:17.1_0": "Agginibbāpano nāma,",
  "tha-ap127:17.2_0": "kappānaṁ chaḷasītiyā;",
  "tha-ap127:17.3_0": "Sattaratanasampanno,",
  "tha-ap127:17.4_0": "cakkavattī mahabbalo.",
  "tha-ap127:18.1_0": "Gatipacchedano nāma,",
  "tha-ap127:18.2_0": "kappānaṁ pañcasītiyā;",
  "tha-ap127:18.3_0": "Sattaratanasampanno,",
  "tha-ap127:18.4_0": "cakkavattī mahabbalo.",
  "tha-ap127:19.1_0": "Rājā vātasamo nāma,",
  "tha-ap127:19.2_0": "kappānaṁ cullasītiyā;",
  "tha-ap127:19.3_0": "Sattaratanasampanno,",
  "tha-ap127:19.4_0": "cakkavattī mahabbalo.",
  "tha-ap127:20.1_0": "Ratanapajjalo nāma,",
  "tha-ap127:20.2_0": "kappānaṁ teasītiyā;",
  "tha-ap127:20.3_0": "Sattaratanasampanno,",
  "tha-ap127:20.4_0": "cakkavattī mahabbalo.",
  "tha-ap127:21.1_0": "Padavikkamano nāma,",
  "tha-ap127:21.2_0": "kappānaṁ dveasītiyā;",
  "tha-ap127:21.3_0": "Sattaratanasampanno,",
  "tha-ap127:21.4_0": "cakkavattī mahabbalo.",
  "tha-ap127:22.1_0": "Rājā vilokano nāma,",
  "tha-ap127:22.2_0": "kappānaṁ ekasītiyā;",
  "tha-ap127:22.3_0": "Sattaratanasampanno,",
  "tha-ap127:22.4_0": "cakkavattī mahabbalo.",
  "tha-ap127:23.1_0": "Girasāroti nāmena,",
  "tha-ap127:23.2_0": "kappesītimhi khattiyo;",
  "tha-ap127:23.3_0": "Sattaratanasampanno,",
  "tha-ap127:23.4_0": "cakkavattī mahabbalo.",
  "tha-ap127:24.1_0": "Paṭisambhidā catasso,",
  "tha-ap127:24.2_0": "…pe…",
  "tha-ap127:24.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap127:25.1_0": "Itthaṁ sudaṁ āyasmā sammukhāthaviko thero imā gāthāyo abhāsitthāti.",
  "tha-ap127:26.1_0": "Sammukhāthavikattherassāpadānaṁ pañcamaṁ."
}