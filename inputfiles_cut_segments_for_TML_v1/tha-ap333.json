{
  "tha-ap333:0.1_0": "Therāpadāna",
  "tha-ap333:0.2_0": "Gandhodakavagga",
  "tha-ap333:0.3_0": "1. Gandhadhūpiyattheraapadāna",
  "tha-ap333:1.1_0": "“Siddhatthassa bhagavato,",
  "tha-ap333:1.2_0": "gandhadhūpaṁ adāsahaṁ;",
  "tha-ap333:1.3_0": "Sumanehi paṭicchannaṁ,",
  "tha-ap333:1.4_0": "buddhānucchavikañca taṁ.",
  "tha-ap333:2.1_0": "Kañcanagghiyasaṅkāsaṁ,",
  "tha-ap333:2.2_0": "buddhaṁ lokagganāyakaṁ;",
  "tha-ap333:2.3_0": "Indīvaraṁva jalitaṁ,",
  "tha-ap333:2.4_0": "ādittaṁva hutāsanaṁ.",
  "tha-ap333:3.1_0": "Byagghusabhaṁva pavaraṁ,",
  "tha-ap333:3.2_0": "abhijātaṁva kesariṁ;",
  "tha-ap333:3.3_0": "Nisinnaṁ samaṇānaggaṁ,",
  "tha-ap333:3.4_0": "bhikkhusaṅghapurakkhataṁ.",
  "tha-ap333:4.1_0": "Disvā cittaṁ pasādetvā,",
  "tha-ap333:4.2_0": "paggahetvāna añjaliṁ;",
  "tha-ap333:4.3_0": "Vanditvā satthuno pāde,",
  "tha-ap333:4.4_0": "pakkāmiṁ uttarāmukho.",
  "tha-ap333:5.1_0": "Catunnavutito kappe,",
  "tha-ap333:5.2_0": "yaṁ gandhamadadiṁ tadā;",
  "tha-ap333:5.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap333:5.4_0": "gandhapūjāyidaṁ phalaṁ.",
  "tha-ap333:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap333:6.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap333:6.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap333:6.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap333:7.1_0": "Itthaṁ sudaṁ āyasmā gandhadhūpiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap333:8.1_0": "Gandhadhūpiyattherassāpadānaṁ paṭhamaṁ."
}