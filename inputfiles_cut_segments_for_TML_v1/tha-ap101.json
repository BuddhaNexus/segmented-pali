{
  "tha-ap101:0.1_0": "Therāpadāna",
  "tha-ap101:0.2_0": "Sudhāvagga",
  "tha-ap101:0.3_0": "9. Veyyāvaccakattheraapadāna",
  "tha-ap101:1.1_0": "“Vipassissa bhagavato,",
  "tha-ap101:1.2_0": "mahāpūgagaṇo ahu;",
  "tha-ap101:1.3_0": "Veyyāvaccakaro āsiṁ,",
  "tha-ap101:1.4_0": "sabbakiccesu vāvaṭo.",
  "tha-ap101:2.1_0": "Deyyadhammo ca me natthi,",
  "tha-ap101:2.2_0": "sugatassa mahesino;",
  "tha-ap101:2.3_0": "Avandiṁ satthuno pāde,",
  "tha-ap101:2.4_0": "vippasannena cetasā.",
  "tha-ap101:3.1_0": "Ekanavutito kappe,",
  "tha-ap101:3.2_0": "veyyāvaccaṁ akāsahaṁ;",
  "tha-ap101:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap101:3.4_0": "veyyāvaccassidaṁ phalaṁ.",
  "tha-ap101:4.1_0": "Ito ca aṭṭhame kappe,",
  "tha-ap101:4.2_0": "rājā āsiṁ sucintito;",
  "tha-ap101:4.3_0": "Sattaratanasampanno,",
  "tha-ap101:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap101:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap101:5.2_0": "…pe…",
  "tha-ap101:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap101:6.1_0": "Itthaṁ sudaṁ āyasmā veyyāvaccako thero imā gāthāyo abhāsitthāti.",
  "tha-ap101:7.1_0": "Veyyāvaccakattherassāpadānaṁ navamaṁ."
}