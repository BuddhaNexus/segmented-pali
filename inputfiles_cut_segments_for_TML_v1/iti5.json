{
  "iti5:0.1_0": "Itivuttaka 5",
  "iti5:0.2_0": "Ekakanipāta",
  "iti5:0.3_0": "Paṭhamavagga",
  "iti5:0.4_0": "Makkhasutta",
  "iti5:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti5:2.1_0": "“Ekadhammaṁ, bhikkhave, pajahatha;",
  "iti5:2.2_0": "ahaṁ vo pāṭibhogo anāgāmitāya.",
  "iti5:2.3_0": "Katamaṁ ekadhammaṁ?",
  "iti5:2.4_0": "Makkhaṁ, bhikkhave, ekadhammaṁ pajahatha;",
  "iti5:2.5_0": "ahaṁ vo pāṭibhogo anāgāmitāyā”ti.",
  "iti5:2.6_0": "Etamatthaṁ bhagavā avoca.",
  "iti5:2.7_0": "Tatthetaṁ iti vuccati:",
  "iti5:3.1_0": "“Yena makkhena makkhāse,",
  "iti5:3.2_0": "sattā gacchanti duggatiṁ;",
  "iti5:3.3_0": "Taṁ makkhaṁ sammadaññāya,",
  "iti5:3.4_0": "pajahanti vipassino;",
  "iti5:3.5_0": "Pahāya na punāyanti,",
  "iti5:3.6_0": "imaṁ lokaṁ kudācanan”ti.",
  "iti5:4.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti5:4.2_0": "Pañcamaṁ."
}