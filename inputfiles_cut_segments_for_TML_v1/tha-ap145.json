{
  "tha-ap145:0.1_0": "Therāpadāna",
  "tha-ap145:0.2_0": "Chattavagga",
  "tha-ap145:0.3_0": "3. Vedikārakattheraapadāna",
  "tha-ap145:1.1_0": "“Nibbute lokanāthamhi,",
  "tha-ap145:1.2_0": "piyadassīnaruttame;",
  "tha-ap145:1.3_0": "Pasannacitto sumano,",
  "tha-ap145:1.4_0": "muttāvedimakāsahaṁ.",
  "tha-ap145:2.1_0": "Maṇīhi parivāretvā,",
  "tha-ap145:2.2_0": "akāsiṁ vedimuttamaṁ;",
  "tha-ap145:2.3_0": "Vedikāya mahaṁ katvā,",
  "tha-ap145:2.4_0": "tattha kālaṅkato ahaṁ.",
  "tha-ap145:3.1_0": "Yaṁ yaṁ yonupapajjāmi,",
  "tha-ap145:3.2_0": "devattaṁ atha mānusaṁ;",
  "tha-ap145:3.3_0": "Maṇī dhārenti ākāse,",
  "tha-ap145:3.4_0": "puññakammassidaṁ phalaṁ.",
  "tha-ap145:4.1_0": "Soḷaseto kappasate,",
  "tha-ap145:4.2_0": "maṇippabhāsanāmakā;",
  "tha-ap145:4.3_0": "Chattiṁsāsiṁsu rājāno,",
  "tha-ap145:4.4_0": "cakkavattī mahabbalā.",
  "tha-ap145:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap145:5.2_0": "…pe…",
  "tha-ap145:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap145:6.1_0": "Itthaṁ sudaṁ āyasmā vedikārako thero imā gāthāyo abhāsitthāti.",
  "tha-ap145:7.1_0": "Vedikārakattherassāpadānaṁ tatiyaṁ."
}