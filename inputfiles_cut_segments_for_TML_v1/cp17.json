{
  "cp17:0.1_0": "Cariyāpiṭaka",
  "cp17:0.2_0": "Hatthināgavagga",
  "cp17:0.3_0": "Sīlapāramī 7",
  "cp17:0.4_0": "Mātaṅgacariya",
  "cp17:1.1_0": "“Punāparaṁ yadā homi,",
  "cp17:1.2_0": "jaṭilo uggatāpano;",
  "cp17:1.3_0": "Mātaṅgo nāma nāmena,",
  "cp17:1.4_0": "sīlavā susamāhito.",
  "cp17:2.1_0": "Ahañca brāhmaṇo eko,",
  "cp17:2.2_0": "gaṅgākūle vasāmubho;",
  "cp17:2.3_0": "Ahaṁ vasāmi upari,",
  "cp17:2.4_0": "heṭṭhā vasati brāhmaṇo.",
  "cp17:3.1_0": "Vicaranto anukūlamhi,",
  "cp17:3.2_0": "uddhaṁ me assamaddasa;",
  "cp17:3.3_0": "Tattha maṁ paribhāsetvā,",
  "cp17:3.4_0": "abhisapi muddhaphālanaṁ.",
  "cp17:4.1_0": "Yadihaṁ tassa pakuppeyyaṁ,",
  "cp17:4.2_0": "yadi sīlaṁ na gopaye;",
  "cp17:4.3_0": "Oloketvānahaṁ tassa,",
  "cp17:4.4_0": "kareyyaṁ chārikaṁ viya.",
  "cp17:5.1_0": "Yaṁ so tadā maṁ abhisapi,",
  "cp17:5.2_0": "kupito duṭṭhamānaso;",
  "cp17:5.3_0": "Tasseva matthake nipati,",
  "cp17:5.4_0": "yogena taṁ pamocayiṁ.",
  "cp17:6.1_0": "Anurakkhiṁ mama sīlaṁ,",
  "cp17:6.2_0": "nārakkhiṁ mama jīvitaṁ;",
  "cp17:6.3_0": "Sīlavā hi tadā āsiṁ,",
  "cp17:6.4_0": "bodhiyāyeva kāraṇā”ti.",
  "cp17:7.1_0": "Mātaṅgacariyaṁ sattamaṁ."
}