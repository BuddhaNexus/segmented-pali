{
  "tha-ap330:0.1_0": "Therāpadāna",
  "tha-ap330:0.2_0": "Umāpupphiyavagga",
  "tha-ap330:0.3_0": "8 Sumanāveḷiyattheraapadāna",
  "tha-ap330:1.1_0": "“Vessabhussa bhagavato,",
  "tha-ap330:1.2_0": "lokajeṭṭhassa tādino;",
  "tha-ap330:1.3_0": "Sabbe janā samāgamma,",
  "tha-ap330:1.4_0": "mahāpūjaṁ karonti te.",
  "tha-ap330:2.1_0": "Sudhāya piṇḍaṁ katvāna,",
  "tha-ap330:2.2_0": "āveḷaṁ sumanāyahaṁ;",
  "tha-ap330:2.3_0": "Sīhāsanassa purato,",
  "tha-ap330:2.4_0": "abhiropesahaṁ tadā.",
  "tha-ap330:3.1_0": "Sabbe janā samāgamma,",
  "tha-ap330:3.2_0": "pekkhanti pupphamuttamaṁ;",
  "tha-ap330:3.3_0": "Kenidaṁ pūjitaṁ pupphaṁ,",
  "tha-ap330:3.4_0": "buddhaseṭṭhassa tādino.",
  "tha-ap330:4.1_0": "Tena cittappasādena,",
  "tha-ap330:4.2_0": "nimmānaṁ upapajjahaṁ;",
  "tha-ap330:4.3_0": "Anubhomi sakaṁ kammaṁ,",
  "tha-ap330:4.4_0": "pubbe sukatamattano.",
  "tha-ap330:5.1_0": "Yaṁ yaṁ yonupapajjāmi,",
  "tha-ap330:5.2_0": "devattaṁ atha mānusaṁ;",
  "tha-ap330:5.3_0": "Sabbesānaṁ piyo homi,",
  "tha-ap330:5.4_0": "pupphapūjāyidaṁ phalaṁ.",
  "tha-ap330:6.1_0": "Nābhijānāmi kāyena,",
  "tha-ap330:6.2_0": "vācāya uda cetasā;",
  "tha-ap330:6.3_0": "Saṁyatānaṁ tapassīnaṁ,",
  "tha-ap330:6.4_0": "kataṁ akkositaṁ mayā.",
  "tha-ap330:7.1_0": "Tena sucaritenāhaṁ,",
  "tha-ap330:7.2_0": "cittassa paṇidhīhi ca;",
  "tha-ap330:7.3_0": "Sabbesaṁ pūjito homi,",
  "tha-ap330:7.4_0": "anakkosassidaṁ phalaṁ.",
  "tha-ap330:8.1_0": "Ito ekādase kappe,",
  "tha-ap330:8.2_0": "sahassārosi khattiyo;",
  "tha-ap330:8.3_0": "Sattaratanasampanno,",
  "tha-ap330:8.4_0": "cakkavattī mahabbalo.",
  "tha-ap330:9.1_0": "Paṭisambhidā catasso,",
  "tha-ap330:9.2_0": "…pe…",
  "tha-ap330:9.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap330:10.1_0": "Itthaṁ sudaṁ āyasmā sumanāveḷiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap330:11.1_0": "Sumanāveḷiyattherassāpadānaṁ aṭṭhamaṁ."
}