{
  "thag2.34:0.1_0": "Theragāthā",
  "thag2.34:0.2_0": "Dukanipāta",
  "thag2.34:0.3_0": "Catutthavagga",
  "thag2.34:0.4_0": "2.34. Isidinnattheragāthā",
  "thag2.34:1.1_0": "“Diṭṭhā mayā dhammadharā upāsakā,",
  "thag2.34:1.2_0": "Kāmā aniccā iti bhāsamānā;",
  "thag2.34:1.3_0": "Sārattarattā maṇikuṇḍalesu,",
  "thag2.34:1.4_0": "Puttesu dāresu ca te apekkhā.",
  "thag2.34:2.1_0": "Addhā na jānanti yatodha dhammaṁ,",
  "thag2.34:2.2_0": "Kāmā aniccā iti cāpi āhu;",
  "thag2.34:2.3_0": "Rāgañca tesaṁ na balatthi chettuṁ,",
  "thag2.34:2.4_0": "Tasmā sitā puttadāraṁ dhanañcā”ti.",
  "thag2.34:2.5_0": "… Isidinno thero …."
}