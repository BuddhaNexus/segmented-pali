{
  "ps1.0:0.1_0": "Paṭisambhidāmagga",
  "ps1.0:0.2_0": "1 Mahāvagga",
  "ps1.0:0.3_0": "1.0 Mātikā",
  "ps1.0:0.4_0": "Namo tassa Bhagavato Arahato Sammāsambuddhassa.",
  "ps1.0:1.1_0": "Sotāvadhāne paññā sutamaye ñāṇaṁ.",
  "ps1.0:2.1_0": "Sutvāna saṁvare paññā sīlamaye ñāṇaṁ.",
  "ps1.0:3.1_0": "Saṁvaritvā samādahane paññā samādhibhāvanāmaye ñāṇaṁ.",
  "ps1.0:4.1_0": "Paccayapariggahe paññā dhammaṭṭhitiñāṇaṁ.",
  "ps1.0:5.1_0": "Atītānāgatapaccuppannānaṁ dhammānaṁ saṅkhipitvā vavatthāne paññā sammasane ñāṇaṁ.",
  "ps1.0:6.1_0": "Paccuppannānaṁ dhammānaṁ vipariṇāmānupassane paññā udayabbayānupassane ñāṇaṁ.",
  "ps1.0:7.1_0": "Ārammaṇaṁ paṭisaṅkhā bhaṅgānupassane paññā vipassane ñāṇaṁ.",
  "ps1.0:8.1_0": "Bhayatupaṭṭhāne paññā ādīnave ñāṇaṁ.",
  "ps1.0:9.1_0": "Muñcitukamyatāpaṭisaṅkhāsantiṭṭhanā paññā saṅkhārupekkhāsu ñāṇaṁ.",
  "ps1.0:10.1_0": "Bahiddhā vuṭṭhānavivaṭṭane paññā gotrabhuñāṇaṁ.",
  "ps1.0:11.1_0": "Dubhato vuṭṭhānavivaṭṭane paññā magge ñāṇaṁ.",
  "ps1.0:12.1_0": "Payogappaṭippassaddhi paññā phale ñāṇaṁ.",
  "ps1.0:13.1_0": "Chinnavaṭumānupassane paññā vimuttiñāṇaṁ.",
  "ps1.0:14.1_0": "Tadā samudāgate dhamme passane paññā paccavekkhaṇe ñāṇaṁ.",
  "ps1.0:15.1_0": "Ajjhattavavatthāne paññā vatthunānatte ñāṇaṁ.",
  "ps1.0:16.1_0": "Bahiddhāvavatthāne paññā gocaranānatte ñāṇaṁ.",
  "ps1.0:17.1_0": "Cariyāvavatthāne paññā cariyānānatte ñāṇaṁ.",
  "ps1.0:18.1_0": "Catudhammavavatthāne paññā bhūminānatte ñāṇaṁ.",
  "ps1.0:19.1_0": "Navadhammavavatthāne paññā dhammanānatte ñāṇaṁ.",
  "ps1.0:20.1_0": "Abhiññāpaññā ñātaṭṭhe ñāṇaṁ.",
  "ps1.0:21.1_0": "Pariññāpaññā tīraṇaṭṭhe ñāṇaṁ.",
  "ps1.0:22.1_0": "Pahāne paññā pariccāgaṭṭhe ñāṇaṁ.",
  "ps1.0:23.1_0": "Bhāvanāpaññā ekarasaṭṭhe ñāṇaṁ.",
  "ps1.0:24.1_0": "Sacchikiriyāpaññā phassanaṭṭhe ñāṇaṁ.",
  "ps1.0:25.1_0": "Atthanānatte paññā atthapaṭisambhide ñāṇaṁ.",
  "ps1.0:26.1_0": "Dhammanānatte paññā dhammapaṭisambhide ñāṇaṁ.",
  "ps1.0:27.1_0": "Niruttinānatte paññā niruttipaṭisambhide ñāṇaṁ.",
  "ps1.0:28.1_0": "Paṭibhānanānatte paññā paṭibhānapaṭisambhide ñāṇaṁ.",
  "ps1.0:29.1_0": "Vihāranānatte paññā vihāraṭṭhe ñāṇaṁ.",
  "ps1.0:30.1_0": "Samāpattinānatte paññā samāpattaṭṭhe ñāṇaṁ.",
  "ps1.0:31.1_0": "Vihārasamāpattinānatte paññā vihārasamāpattaṭṭhe ñāṇaṁ.",
  "ps1.0:32.1_0": "Avikkhepaparisuddhattā āsavasamucchede paññā ānantarikasamādhimhi ñāṇaṁ.",
  "ps1.0:33.1_0": "Dassanādhipateyyaṁ santo ca vihārādhigamo paṇītādhimuttatā paññā araṇavihāre ñāṇaṁ.",
  "ps1.0:34.1_0": "Dvīhi balehi samannāgatattā tayo ca saṅkhārānaṁ paṭippassaddhiyā soḷasahi ñāṇacariyāhi navahi samādhicariyāhi vasibhāvatā paññā nirodhasamāpattiyā ñāṇaṁ.",
  "ps1.0:35.1_0": "Sampajānassa pavattapariyādāne paññā parinibbāne ñāṇaṁ.",
  "ps1.0:36.1_0": "Sabbadhammānaṁ sammā samucchede nirodhe ca anupaṭṭhānatā paññā samasīsaṭṭhe ñāṇaṁ.",
  "ps1.0:37.1_0": "Puthunānattatejapariyādāne paññā sallekhaṭṭhe ñāṇaṁ.",
  "ps1.0:38.1_0": "Asallīnattapahitattapaggahaṭṭhe paññā vīriyārambhe ñāṇaṁ.",
  "ps1.0:39.1_0": "Nānādhammappakāsanatā paññā atthasandassane ñāṇaṁ.",
  "ps1.0:40.1_0": "Sabbadhammānaṁ ekasaṅgahatānānattekattapaṭivedhe paññā dassanavisuddhiñāṇaṁ.",
  "ps1.0:41.1_0": "Viditattā paññā khantiñāṇaṁ.",
  "ps1.0:42.1_0": "Phuṭṭhattā paññā pariyogāhaṇe ñāṇaṁ.",
  "ps1.0:43.1_0": "Samodahane paññā padesavihāre ñāṇaṁ.",
  "ps1.0:44.1_0": "Adhipatattā paññā saññāvivaṭṭe ñāṇaṁ.",
  "ps1.0:45.1_0": "Nānatte paññā cetovivaṭṭe ñāṇaṁ.",
  "ps1.0:46.1_0": "Adhiṭṭhāne paññā cittavivaṭṭe ñāṇaṁ.",
  "ps1.0:47.1_0": "Suññate paññā ñāṇavivaṭṭe ñāṇaṁ.",
  "ps1.0:48.1_0": "Vosagge paññā vimokkhavivaṭṭe ñāṇaṁ.",
  "ps1.0:49.1_0": "Tathaṭṭhe paññā saccavivaṭṭe ñāṇaṁ.",
  "ps1.0:50.1_0": "Kāyampi cittampi ekavavatthānatā sukhasaññañca lahusaññañca adhiṭṭhānavasena ijjhanaṭṭhe paññā iddhividhe ñāṇaṁ.",
  "ps1.0:51.1_0": "Vitakkavipphāravasena nānattekattasaddanimittānaṁ pariyogāhaṇe paññā sotadhātuvisuddhiñāṇaṁ.",
  "ps1.0:52.1_0": "Tiṇṇannaṁ cittānaṁ vipphārattā indriyānaṁ pasādavasena nānattekattaviññāṇacariyā pariyogāhaṇe paññā cetopariyañāṇaṁ.",
  "ps1.0:53.1_0": "Paccayappavattānaṁ dhammānaṁ nānattekattakammavipphāravasena pariyogāhaṇe paññā pubbenivāsānussatiñāṇaṁ.",
  "ps1.0:54.1_0": "Obhāsavasena nānattekattarūpanimittānaṁ dassanaṭṭhe paññā dibbacakkhuñāṇaṁ.",
  "ps1.0:55.1_0": "Catusaṭṭhiyā ākārehi tiṇṇannaṁ indriyānaṁ vasibhāvatā paññā āsavānaṁ khaye ñāṇaṁ.",
  "ps1.0:56.1_0": "Pariññaṭṭhe paññā dukkhe ñāṇaṁ.",
  "ps1.0:57.1_0": "Pahānaṭṭhe paññā samudaye ñāṇaṁ.",
  "ps1.0:58.1_0": "Sacchikiriyaṭṭhe paññā nirodhe ñāṇaṁ.",
  "ps1.0:59.1_0": "Bhāvanaṭṭhe paññā magge ñāṇaṁ.",
  "ps1.0:60.1_0": "Dukkhe ñāṇaṁ.",
  "ps1.0:61.1_0": "Dukkhasamudaye ñāṇaṁ.",
  "ps1.0:62.1_0": "Dukkhanirodhe ñāṇaṁ.",
  "ps1.0:63.1_0": "Dukkhanirodhagāminiyā paṭipadāya ñāṇaṁ.",
  "ps1.0:64.1_0": "Atthapaṭisambhide ñāṇaṁ.",
  "ps1.0:65.1_0": "Dhammapaṭisambhide ñāṇaṁ.",
  "ps1.0:66.1_0": "Niruttipaṭisambhide ñāṇaṁ.",
  "ps1.0:67.1_0": "Paṭibhānapaṭisambhide ñāṇaṁ.",
  "ps1.0:68.1_0": "Indriyaparopariyattañāṇaṁ.",
  "ps1.0:69.1_0": "Sattānaṁ āsayānusaye ñāṇaṁ.",
  "ps1.0:70.1_0": "Yamakapāṭihīre ñāṇaṁ.",
  "ps1.0:71.1_0": "Mahākaruṇāsamāpattiyā ñāṇaṁ.",
  "ps1.0:72.1_0": "Sabbaññutaññāṇaṁ.",
  "ps1.0:73.1_0": "Anāvaraṇañāṇaṁ.",
  "ps1.0:74.1_0": "Imāni tesattati ñāṇāni.",
  "ps1.0:74.2_0": "Imesaṁ tesattatiyā ñāṇānaṁ sattasaṭṭhi ñāṇāni sāvakasādhāraṇāni;",
  "ps1.0:74.3_0": "cha ñāṇāni asādhāraṇāni sāvakehi.",
  "ps1.0:74.4_0": "Mātikā niṭṭhitā."
}