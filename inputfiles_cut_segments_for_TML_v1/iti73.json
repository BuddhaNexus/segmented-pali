{
  "iti73:0.1_0": "Itivuttaka 73",
  "iti73:0.2_0": "Tikanipāta",
  "iti73:0.3_0": "Tatiyavagga",
  "iti73:0.4_0": "Santatarasutta",
  "iti73:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti73:2.1_0": "“Rūpehi, bhikkhave, arūpā santatarā, arūpehi nirodho santataro”ti.",
  "iti73:2.2_0": "Etamatthaṁ bhagavā avoca.",
  "iti73:2.3_0": "Tatthetaṁ iti vuccati:",
  "iti73:3.1_0": "“Ye ca rūpūpagā sattā,",
  "iti73:3.2_0": "ye ca arūpaṭṭhāyino;",
  "iti73:3.3_0": "Nirodhaṁ appajānantā,",
  "iti73:3.4_0": "āgantāro punabbhavaṁ.",
  "iti73:4.1_0": "Ye ca rūpe pariññāya,",
  "iti73:4.2_0": "arūpesu asaṇṭhitā;",
  "iti73:4.3_0": "Nirodhe ye vimuccanti,",
  "iti73:4.4_0": "te janā maccuhāyino.",
  "iti73:5.1_0": "Kāyena amatadhātuṁ,",
  "iti73:5.2_0": "phusayitvā nirūpadhiṁ;",
  "iti73:5.3_0": "Upadhippaṭinissaggaṁ,",
  "iti73:5.4_0": "sacchikatvā anāsavo;",
  "iti73:5.5_0": "Deseti sammāsambuddho,",
  "iti73:5.6_0": "asokaṁ virajaṁ padan”ti.",
  "iti73:6.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti73:6.2_0": "Catutthaṁ."
}