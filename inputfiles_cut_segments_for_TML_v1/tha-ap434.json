{
  "tha-ap434:0.1_0": "Therāpadāna",
  "tha-ap434:0.2_0": "Ekavihārivagga",
  "tha-ap434:0.3_0": "2. Ekasaṅkhiyattheraapadāna",
  "tha-ap434:1.1_0": "“Vipassino bhagavato,",
  "tha-ap434:1.2_0": "mahābodhimaho ahu;",
  "tha-ap434:1.3_0": "Mahājanā samāgamma,",
  "tha-ap434:1.4_0": "pūjenti bodhimuttamaṁ.",
  "tha-ap434:2.1_0": "Na hi taṁ orakaṁ maññe,",
  "tha-ap434:2.2_0": "buddhaseṭṭho bhavissati;",
  "tha-ap434:2.3_0": "Yassāyaṁ īdisā bodhi,",
  "tha-ap434:2.4_0": "pūjanīyā ca satthuno.",
  "tha-ap434:3.1_0": "Tato saṅkhaṁ gahetvāna,",
  "tha-ap434:3.2_0": "bodhirukkhamupaṭṭhahiṁ;",
  "tha-ap434:3.3_0": "Dhamanto sabbadivasaṁ,",
  "tha-ap434:3.4_0": "avandiṁ bodhimuttamaṁ.",
  "tha-ap434:4.1_0": "Āsannake kataṁ kammaṁ,",
  "tha-ap434:4.2_0": "devalokaṁ apāpayī;",
  "tha-ap434:4.3_0": "Kaḷevaraṁ me patitaṁ,",
  "tha-ap434:4.4_0": "devaloke ramāmahaṁ.",
  "tha-ap434:5.1_0": "Saṭṭhituriyasahassāni,",
  "tha-ap434:5.2_0": "tuṭṭhahaṭṭhā pamoditā;",
  "tha-ap434:5.3_0": "Sadā mayhaṁ upaṭṭhanti,",
  "tha-ap434:5.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap434:6.1_0": "Ekasattatime kappe,",
  "tha-ap434:6.2_0": "rājā āsiṁ sudassano;",
  "tha-ap434:6.3_0": "Cāturanto vijitāvī,",
  "tha-ap434:6.4_0": "jambumaṇḍassa issaro.",
  "tha-ap434:7.1_0": "Tato aṅgasatā turiyā,",
  "tha-ap434:7.2_0": "parivārenti maṁ sadā;",
  "tha-ap434:7.3_0": "Anubhomi sakaṁ kammaṁ,",
  "tha-ap434:7.4_0": "upaṭṭhānassidaṁ phalaṁ.",
  "tha-ap434:8.1_0": "Yaṁ yaṁ yonupapajjāmi,",
  "tha-ap434:8.2_0": "devattaṁ atha mānusaṁ;",
  "tha-ap434:8.3_0": "Mātukucchigatassāpi,",
  "tha-ap434:8.4_0": "vajjare bheriyo sadā.",
  "tha-ap434:9.1_0": "Upaṭṭhitvāna sambuddhaṁ,",
  "tha-ap434:9.2_0": "anubhutvāna sampadā;",
  "tha-ap434:9.3_0": "Sivaṁ sukhemaṁ amataṁ,",
  "tha-ap434:9.4_0": "pattomhi acalaṁ padaṁ.",
  "tha-ap434:10.1_0": "Ekanavutito kappe,",
  "tha-ap434:10.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap434:10.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap434:10.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap434:11.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap434:11.2_0": "…pe…",
  "tha-ap434:11.3_0": "viharāmi anāsavo.",
  "tha-ap434:12.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap434:12.2_0": "…pe…",
  "tha-ap434:12.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap434:13.1_0": "Paṭisambhidā catasso,",
  "tha-ap434:13.2_0": "…pe…",
  "tha-ap434:13.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap434:14.1_0": "Itthaṁ sudaṁ āyasmā ekasaṅkhiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap434:15.1_0": "Ekasaṅkhiyattherassāpadānaṁ dutiyaṁ."
}