{
  "kv16.4:0.1_0": "Kathāvatthu",
  "kv16.4:0.2_0": "Mahāpaṇṇāsaka",
  "kv16.4:0.3_0": "Soḷasamavagga",
  "kv16.4:0.4_0": "Adhigayhamanasikārakathā",
  "kv16.4:1.1_0": "Adhigayha manasi karotīti?",
  "kv16.4:1.2_0": "Āmantā.",
  "kv16.4:1.3_0": "Tena cittena taṁ cittaṁ pajānātīti?",
  "kv16.4:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv16.4:1.5_0": "tena cittena taṁ cittaṁ pajānātīti?",
  "kv16.4:1.6_0": "Āmantā.",
  "kv16.4:1.7_0": "Tena cittena taṁ cittaṁ “cittan”ti pajānātīti?",
  "kv16.4:1.8_0": "Na hevaṁ vattabbe …pe…",
  "kv16.4:1.9_0": "tena cittena taṁ cittaṁ “cittan”ti pajānātīti?",
  "kv16.4:1.10_0": "Āmantā.",
  "kv16.4:1.11_0": "Taṁ cittaṁ tassa cittassa ārammaṇanti?",
  "kv16.4:1.12_0": "Na hevaṁ vattabbe …pe….",
  "kv16.4:2.1_0": "Taṁ cittaṁ tassa cittassa ārammaṇanti?",
  "kv16.4:2.2_0": "Āmantā.",
  "kv16.4:2.3_0": "Tena phassena taṁ phassaṁ phusati, tāya vedanāya …pe…",
  "kv16.4:2.4_0": "tāya saññāya …",
  "kv16.4:2.5_0": "tāya cetanāya …",
  "kv16.4:2.6_0": "tena cittena …",
  "kv16.4:2.7_0": "tena vitakkena …",
  "kv16.4:2.8_0": "tena vicārena …",
  "kv16.4:2.9_0": "tāya pītiyā …",
  "kv16.4:2.10_0": "tāya satiyā …",
  "kv16.4:2.11_0": "tāya paññāya taṁ paññaṁ pajānātīti?",
  "kv16.4:2.12_0": "Na hevaṁ vattabbe …pe….",
  "kv16.4:3.1_0": "Atītaṁ “atītan”ti manasikaronto, anāgataṁ “anāgatan”ti manasi karotīti?",
  "kv16.4:3.2_0": "Na hevaṁ vattabbe …pe…",
  "kv16.4:3.3_0": "atītaṁ “atītan”ti manasikaronto, anāgataṁ “anāgatan”ti manasi karotīti?",
  "kv16.4:3.4_0": "Āmantā.",
  "kv16.4:3.5_0": "Dvinnaṁ phassānaṁ …pe…",
  "kv16.4:3.6_0": "dvinnaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv16.4:3.7_0": "Na hevaṁ vattabbe …pe….",
  "kv16.4:4.1_0": "Atītaṁ “atītan”ti manasikaronto, paccuppannaṁ “paccuppannan”ti manasi karotīti?",
  "kv16.4:4.2_0": "Na hevaṁ vattabbe …pe…",
  "kv16.4:4.3_0": "atītaṁ “atītan”ti manasikaronto, paccuppannaṁ “paccuppannan”ti manasi karotīti?",
  "kv16.4:4.4_0": "Āmantā.",
  "kv16.4:4.5_0": "Dvinnaṁ phassānaṁ …pe…",
  "kv16.4:4.6_0": "dvinnaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv16.4:4.7_0": "Na hevaṁ vattabbe …pe….",
  "kv16.4:5.1_0": "Atītaṁ “atītan”ti manasikaronto, anāgataṁ “anāgatan”ti manasi karoti, paccuppannaṁ “paccuppannan”ti manasi karotīti?",
  "kv16.4:5.2_0": "Na hevaṁ vattabbe …pe…",
  "kv16.4:5.3_0": "atītaṁ “atītan”ti manasikaronto, anāgataṁ “anāgatan”ti manasi karoti, paccuppannaṁ “paccuppannan”ti manasi karotīti?",
  "kv16.4:5.4_0": "Āmantā.",
  "kv16.4:5.5_0": "Tiṇṇaṁ phassānaṁ …pe…",
  "kv16.4:5.6_0": "tiṇṇaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv16.4:5.7_0": "Na hevaṁ vattabbe …pe….",
  "kv16.4:6.1_0": "Anāgataṁ “anāgatan”ti manasikaronto, atītaṁ “atītan”ti manasi karotīti?",
  "kv16.4:6.2_0": "Na hevaṁ vattabbe …pe…",
  "kv16.4:6.3_0": "anāgataṁ “anāgatan”ti manasikaronto, atītaṁ “atītan”ti manasi karotīti?",
  "kv16.4:6.4_0": "Āmantā.",
  "kv16.4:6.5_0": "Dvinnaṁ phassānaṁ …pe…",
  "kv16.4:6.6_0": "dvinnaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv16.4:6.7_0": "Na hevaṁ vattabbe …pe….",
  "kv16.4:7.1_0": "Anāgataṁ “anāgatan”ti manasikaronto, paccuppannaṁ “paccuppannan”ti manasi karotīti?",
  "kv16.4:7.2_0": "Na hevaṁ vattabbe …pe…",
  "kv16.4:7.3_0": "anāgataṁ “anāgatan”ti manasikaronto, paccuppannaṁ “paccuppannan”ti manasi karotīti?",
  "kv16.4:7.4_0": "Āmantā.",
  "kv16.4:7.5_0": "Dvinnaṁ phassānaṁ …pe…",
  "kv16.4:7.6_0": "dvinnaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv16.4:7.7_0": "Na hevaṁ vattabbe …pe….",
  "kv16.4:8.1_0": "Anāgataṁ “anāgatan”ti manasikaronto, atītaṁ “atītan”ti manasi karoti, paccuppannaṁ “paccuppannan”ti manasi karotīti?",
  "kv16.4:8.2_0": "Na hevaṁ vattabbe …pe…",
  "kv16.4:8.3_0": "anāgataṁ “anāgatan”ti manasikaronto, atītaṁ “atītan”ti manasi karoti, paccuppannaṁ “paccuppannan”ti manasi karotīti?",
  "kv16.4:8.4_0": "Āmantā.",
  "kv16.4:8.5_0": "Tiṇṇaṁ phassānaṁ …pe…",
  "kv16.4:8.6_0": "tiṇṇaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv16.4:8.7_0": "Na hevaṁ vattabbe …pe….",
  "kv16.4:9.1_0": "Paccuppannaṁ “paccuppannan”ti manasikaronto, atītaṁ “atītan”ti manasi karotīti?",
  "kv16.4:9.2_0": "Na hevaṁ vattabbe …pe…",
  "kv16.4:9.3_0": "paccuppannaṁ “paccuppannan”ti manasikaronto, atītaṁ “atītan”ti manasi karotīti?",
  "kv16.4:9.4_0": "Āmantā.",
  "kv16.4:9.5_0": "Dvinnaṁ phassānaṁ …pe…",
  "kv16.4:9.6_0": "dvinnaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv16.4:9.7_0": "Na hevaṁ vattabbe …pe….",
  "kv16.4:10.1_0": "Paccuppannaṁ “paccuppannan”ti manasikaronto, anāgataṁ “anāgatan”ti manasi karotīti?",
  "kv16.4:10.2_0": "Na hevaṁ vattabbe …pe…",
  "kv16.4:10.3_0": "paccuppannaṁ “paccuppannan”ti manasikaronto, anāgataṁ “anāgatan”ti manasi karotīti?",
  "kv16.4:10.4_0": "Āmantā.",
  "kv16.4:10.5_0": "Dvinnaṁ phassānaṁ …pe…",
  "kv16.4:10.6_0": "dvinnaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv16.4:10.7_0": "Na hevaṁ vattabbe …pe….",
  "kv16.4:11.1_0": "Paccuppannaṁ “paccuppannan”ti manasikaronto, atītaṁ “atītan”ti manasi karoti, anāgataṁ “anāgatan”ti manasi karotīti?",
  "kv16.4:11.2_0": "Na hevaṁ vattabbe …pe…",
  "kv16.4:11.3_0": "paccuppannaṁ “paccuppannan”ti manasikaronto, atītaṁ “atītan”ti manasi karoti, anāgataṁ “anāgatan”ti manasi karotīti?",
  "kv16.4:11.4_0": "Āmantā.",
  "kv16.4:11.5_0": "Tiṇṇaṁ phassānaṁ …pe…",
  "kv16.4:11.6_0": "tiṇṇaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv16.4:11.7_0": "Na hevaṁ vattabbe …pe….",
  "kv16.4:12.1_0": "Na vattabbaṁ—",
  "kv16.4:12.2_0": "“adhigayha manasi karotī”ti?",
  "kv16.4:12.3_0": "Āmantā.",
  "kv16.4:12.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv16.4:13.1_0": "“Sabbe saṅkhārā aniccāti,",
  "kv16.4:13.2_0": "yadā paññāya passati;",
  "kv16.4:13.3_0": "Atha nibbindati dukkhe,",
  "kv16.4:13.4_0": "esa maggo visuddhiyā.",
  "kv16.4:14.1_0": "Sabbe saṅkhārā dukkhāti,",
  "kv16.4:14.2_0": "yadā paññāya passati;",
  "kv16.4:14.3_0": "Atha nibbindati dukkhe,",
  "kv16.4:14.4_0": "esa maggo visuddhiyā.",
  "kv16.4:15.1_0": "Sabbe dhammā anattāti,",
  "kv16.4:15.2_0": "yadā paññāya passati;",
  "kv16.4:15.3_0": "Atha nibbindati dukkhe,",
  "kv16.4:15.4_0": "esa maggo visuddhiyā”ti.",
  "kv16.4:16.1_0": "Attheva suttantoti?",
  "kv16.4:16.2_0": "Āmantā.",
  "kv16.4:16.3_0": "Tena hi adhigayha manasi karotīti.",
  "kv16.4:16.4_0": "Adhigayhamanasikārakathā niṭṭhitā."
}