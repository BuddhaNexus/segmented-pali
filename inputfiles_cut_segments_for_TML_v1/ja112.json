{
  "ja112:0.1_0": "Jātaka",
  "ja112:0.2_0": "Ekakanipāta",
  "ja112:0.3_0": "Haṁcivagga",
  "ja112:0.4_0": "2. Amarādevīpañhajātaka",
  "ja112:1.1_0": "“Yena sattubilaṅgā ca,",
  "ja112:1.2_0": "Diguṇapalāso ca pupphito;",
  "ja112:1.3_0": "Yena dadāmi tena vadāmi,",
  "ja112:1.4_0": "Yena na dadāmi na tena vadāmi;",
  "ja112:1.5_0": "Esa maggo yavamajjhakassa,",
  "ja112:1.6_0": "Etaṁ channapathaṁ vijānāhī”ti.",
  "ja112:2.1_0": "Amarādevīpañhajātakaṁ dutiyaṁ."
}