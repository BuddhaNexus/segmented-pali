{
  "tha-ap171:0.1_0": "Therāpadāna",
  "tha-ap171:0.2_0": "Supāricariyavagga",
  "tha-ap171:0.3_0": "9. Samādapakattheraapadāna",
  "tha-ap171:1.1_0": "“Nagare bandhumatiyā,",
  "tha-ap171:1.2_0": "mahāpūgagaṇo ahu;",
  "tha-ap171:1.3_0": "Tesāhaṁ pavaro āsiṁ,",
  "tha-ap171:1.4_0": "mama baddhacarā ca te.",
  "tha-ap171:2.1_0": "Sabbe te sannipātetvā,",
  "tha-ap171:2.2_0": "puññakamme samādayiṁ;",
  "tha-ap171:2.3_0": "Māḷaṁ kassāma saṅghassa,",
  "tha-ap171:2.4_0": "puññakkhettaṁ anuttaraṁ.",
  "tha-ap171:3.1_0": "Sādhūti te paṭissutvā,",
  "tha-ap171:3.2_0": "mama chandavasānugā;",
  "tha-ap171:3.3_0": "Niṭṭhāpetvā ca taṁ māḷaṁ,",
  "tha-ap171:3.4_0": "vipassissa adamhase.",
  "tha-ap171:4.1_0": "Ekanavutito kappe,",
  "tha-ap171:4.2_0": "yaṁ māḷamadadiṁ tadā;",
  "tha-ap171:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap171:4.4_0": "māḷadānassidaṁ phalaṁ.",
  "tha-ap171:5.1_0": "Ekūnasattatikappe,",
  "tha-ap171:5.2_0": "eko āsi janādhipo;",
  "tha-ap171:5.3_0": "Ādeyyo nāma nāmena,",
  "tha-ap171:5.4_0": "cakkavattī mahabbalo.",
  "tha-ap171:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap171:6.2_0": "…pe…",
  "tha-ap171:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap171:7.1_0": "Itthaṁ sudaṁ āyasmā samādapako thero imā gāthāyo abhāsitthāti.",
  "tha-ap171:8.1_0": "Samādapakattherassāpadānaṁ navamaṁ."
}