{
  "tha-ap436:0.1_0": "Therāpadāna",
  "tha-ap436:0.2_0": "Ekavihārivagga",
  "tha-ap436:0.3_0": "4. Ñāṇatthavikattheraapadāna",
  "tha-ap436:1.1_0": "“Kaṇikāraṁva jalitaṁ,",
  "tha-ap436:1.2_0": "dīparukkhaṁva jotitaṁ;",
  "tha-ap436:1.3_0": "Kañcanaṁva virocantaṁ,",
  "tha-ap436:1.4_0": "addasaṁ dvipaduttamaṁ.",
  "tha-ap436:2.1_0": "Kamaṇḍaluṁ ṭhapetvāna,",
  "tha-ap436:2.2_0": "vākacīrañca kuṇḍikaṁ;",
  "tha-ap436:2.3_0": "Ekaṁsaṁ ajinaṁ katvā,",
  "tha-ap436:2.4_0": "buddhaseṭṭhaṁ thaviṁ ahaṁ.",
  "tha-ap436:3.1_0": "‘Tamandhakāraṁ vidhamaṁ,",
  "tha-ap436:3.2_0": "mohajālasamākulaṁ;",
  "tha-ap436:3.3_0": "Ñāṇālokaṁ dassetvāna,",
  "tha-ap436:3.4_0": "nittiṇṇosi mahāmuni.",
  "tha-ap436:4.1_0": "Samuddharasimaṁ lokaṁ,",
  "tha-ap436:4.2_0": "sabbāvantamanuttaraṁ;",
  "tha-ap436:4.3_0": "Ñāṇe te upamā natthi,",
  "tha-ap436:4.4_0": "yāvatājagatogati.",
  "tha-ap436:5.1_0": "Tena ñāṇena sabbaññū,",
  "tha-ap436:5.2_0": "iti buddho pavuccati;",
  "tha-ap436:5.3_0": "Vandāmi taṁ mahāvīraṁ,",
  "tha-ap436:5.4_0": "sabbaññutamanāvaraṁ’.",
  "tha-ap436:6.1_0": "Satasahassito kappe,",
  "tha-ap436:6.2_0": "buddhaseṭṭhaṁ thaviṁ ahaṁ;",
  "tha-ap436:6.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap436:6.4_0": "ñāṇatthavāyidaṁ phalaṁ.",
  "tha-ap436:7.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap436:7.2_0": "…pe…",
  "tha-ap436:7.3_0": "viharāmi anāsavo.",
  "tha-ap436:8.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap436:8.2_0": "…pe…",
  "tha-ap436:8.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap436:9.1_0": "Paṭisambhidā catasso,",
  "tha-ap436:9.2_0": "…pe…",
  "tha-ap436:9.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap436:10.1_0": "Itthaṁ sudaṁ āyasmā ñāṇatthaviko thero imā gāthāyo abhāsitthāti.",
  "tha-ap436:11.1_0": "Ñāṇatthavikattherassāpadānaṁ catutthaṁ."
}