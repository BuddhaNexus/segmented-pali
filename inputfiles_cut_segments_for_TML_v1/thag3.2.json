{
  "thag3.2:0.1_0": "Theragāthā",
  "thag3.2:0.2_0": "Tikanipāta",
  "thag3.2:0.3_0": "Paṭhamavagga",
  "thag3.2:0.4_0": "3.2. Paccayattheragāthā",
  "thag3.2:1.1_0": "“Pañcāhāhaṁ pabbajito,",
  "thag3.2:1.2_0": "sekho appattamānaso;",
  "thag3.2:1.3_0": "Vihāraṁ me paviṭṭhassa,",
  "thag3.2:1.4_0": "cetaso paṇidhī ahu.",
  "thag3.2:2.1_0": "Nāsissaṁ na pivissāmi,",
  "thag3.2:2.2_0": "vihārato na nikkhame;",
  "thag3.2:2.3_0": "Napi passaṁ nipātessaṁ,",
  "thag3.2:2.4_0": "taṇhāsalle anūhate.",
  "thag3.2:3.1_0": "Tassa mevaṁ viharato,",
  "thag3.2:3.2_0": "passa vīriyaparakkamaṁ;",
  "thag3.2:3.3_0": "Tisso vijjā anuppattā,",
  "thag3.2:3.4_0": "kataṁ buddhassa sāsanan”ti.",
  "thag3.2:3.5_0": "… Paccayo thero …."
}