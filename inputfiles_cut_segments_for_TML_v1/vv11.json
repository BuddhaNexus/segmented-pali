{
  "vv11:0.1_0": "Vimānavatthu",
  "vv11:0.2_0": "Itthivimāna",
  "vv11:0.3_0": "Pīṭhavagga",
  "vv11:0.4_0": "11. Paṭhamapatibbatāvimānavatthu",
  "vv11:1.1_0": "“Koñcā mayūrā diviyā ca haṁsā,",
  "vv11:1.2_0": "Vaggussarā kokilā sampatanti;",
  "vv11:1.3_0": "Pupphābhikiṇṇaṁ rammamidaṁ vimānaṁ,",
  "vv11:1.4_0": "Anekacittaṁ naranārisevitaṁ.",
  "vv11:2.1_0": "Tatthacchasi devi mahānubhāve,",
  "vv11:2.2_0": "Iddhī vikubbanti anekarūpā;",
  "vv11:2.3_0": "Imā ca te accharāyo samantato,",
  "vv11:2.4_0": "Naccanti gāyanti pamodayanti ca.",
  "vv11:3.1_0": "Deviddhipattāsi mahānubhāve,",
  "vv11:3.2_0": "Manussabhūtā kimakāsi puññaṁ;",
  "vv11:3.3_0": "Kenāsi evaṁ jalitānubhāvā,",
  "vv11:3.4_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv11:4.1_0": "Sā devatā attamanā,",
  "vv11:4.2_0": "moggallānena pucchitā;",
  "vv11:4.3_0": "Pañhaṁ puṭṭhā viyākāsi,",
  "vv11:4.4_0": "yassa kammassidaṁ phalaṁ.",
  "vv11:5.1_0": "“Ahaṁ manussesu manussabhūtā,",
  "vv11:5.2_0": "Patibbatānaññamanā ahosiṁ;",
  "vv11:5.3_0": "Mātāva puttaṁ anurakkhamānā,",
  "vv11:5.4_0": "Kuddhāpihaṁ nappharusaṁ avocaṁ.",
  "vv11:6.1_0": "Sacce ṭhitā mosavajjaṁ pahāya,",
  "vv11:6.2_0": "Dāne ratā saṅgahitattabhāvā;",
  "vv11:6.3_0": "Annañca pānañca pasannacittā,",
  "vv11:6.4_0": "Sakkacca dānaṁ vipulaṁ adāsiṁ.",
  "vv11:7.1_0": "Tena metādiso vaṇṇo,",
  "vv11:7.2_0": "tena me idha mijjhati;",
  "vv11:7.3_0": "Uppajjanti ca me bhogā,",
  "vv11:7.4_0": "ye keci manaso piyā.",
  "vv11:8.1_0": "Akkhāmi te bhikkhu mahānubhāva,",
  "vv11:8.2_0": "Manussabhūtā yamakāsi puññaṁ;",
  "vv11:8.3_0": "Tenamhi evaṁ jalitānubhāvā,",
  "vv11:8.4_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv11:9.1_0": "Paṭhamapatibbatāvimānaṁ ekādasamaṁ."
}