{
  "ja147:0.1_0": "Jātaka",
  "ja147:0.2_0": "Ekakanipāta",
  "ja147:0.3_0": "Kakaṇṭakavagga",
  "ja147:0.4_0": "7. Puppharattajātaka",
  "ja147:1.1_0": "“Nayidaṁ dukkhaṁ aduṁ dukkhaṁ,",
  "ja147:1.2_0": "Yaṁ maṁ tudati vāyaso;",
  "ja147:1.3_0": "Yaṁ sāmā puppharattena,",
  "ja147:1.4_0": "Kattikaṁ nānubhossatī”ti.",
  "ja147:2.1_0": "Puppharattajātakaṁ sattamaṁ."
}