{
  "tha-ap131:0.1_0": "Therāpadāna",
  "tha-ap131:0.2_0": "Sereyyavagga",
  "tha-ap131:0.3_0": "9 Gaṇṭhipupphiyattheraapadāna",
  "tha-ap131:1.1_0": "“Suvaṇṇavaṇṇo sambuddho,",
  "tha-ap131:1.2_0": "vipassī dakkhiṇāraho;",
  "tha-ap131:1.3_0": "Purakkhato sāvakehi,",
  "tha-ap131:1.4_0": "ārāmā abhinikkhami.",
  "tha-ap131:2.1_0": "Disvānahaṁ buddhaseṭṭhaṁ,",
  "tha-ap131:2.2_0": "sabbaññuṁ tamanāsakaṁ;",
  "tha-ap131:2.3_0": "Pasannacitto sumano,",
  "tha-ap131:2.4_0": "gaṇṭhipupphaṁ apūjayiṁ.",
  "tha-ap131:3.1_0": "Tena cittappasādena,",
  "tha-ap131:3.2_0": "dvipadindassa tādino;",
  "tha-ap131:3.3_0": "Haṭṭho haṭṭhena cittena,",
  "tha-ap131:3.4_0": "puna vandiṁ tathāgataṁ.",
  "tha-ap131:4.1_0": "Ekanavutito kappe,",
  "tha-ap131:4.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap131:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap131:4.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap131:5.1_0": "Ekatālīsito kappe,",
  "tha-ap131:5.2_0": "caraṇo nāma khattiyo;",
  "tha-ap131:5.3_0": "Sattaratanasampanno,",
  "tha-ap131:5.4_0": "cakkavattī mahabbalo.",
  "tha-ap131:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap131:6.2_0": "…pe…",
  "tha-ap131:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap131:7.1_0": "Itthaṁ sudaṁ āyasmā gaṇṭhipupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap131:8.1_0": "Gaṇṭhipupphiyattherassāpadānaṁ navamaṁ."
}