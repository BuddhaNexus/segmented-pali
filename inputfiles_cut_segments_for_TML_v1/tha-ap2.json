{
  "tha-ap2:0.1_0": "Therāpadāna",
  "tha-ap2:0.2_0": "Buddhavagga",
  "tha-ap2:0.3_0": "2. Paccekabuddhaapadāna",
  "tha-ap2:1.1_0": "Atha paccekabuddhāpadānaṁ suṇātha—",
  "tha-ap2:2.1_0": "“Tathāgataṁ jetavane vasantaṁ,",
  "tha-ap2:2.2_0": "Apucchi vedehamunī nataṅgo;",
  "tha-ap2:2.3_0": "‘Paccekabuddhā kira nāma honti,",
  "tha-ap2:2.4_0": "Bhavanti te hetubhi kehi vīra’.",
  "tha-ap2:3.1_0": "Tadāha sabbaññuvaro mahesi,",
  "tha-ap2:3.2_0": "Ānandabhaddaṁ madhurassarena;",
  "tha-ap2:3.3_0": "‘Ye pubbabuddhesu katādhikārā,",
  "tha-ap2:3.4_0": "Aladdhamokkhā jinasāsanesu.",
  "tha-ap2:4.1_0": "Teneva saṁvegamukhena dhīrā,",
  "tha-ap2:4.2_0": "Vināpi buddhehi sutikkhapaññā;",
  "tha-ap2:4.3_0": "Ārammaṇenāpi parittakena,",
  "tha-ap2:4.4_0": "Paccekabodhiṁ anupāpuṇanti.",
  "tha-ap2:5.1_0": "Sabbamhi lokamhi mamaṁ ṭhapetvā,",
  "tha-ap2:5.2_0": "Paccekabuddhehi samova natthi;",
  "tha-ap2:5.3_0": "Tesaṁ imaṁ vaṇṇapadesamattaṁ,",
  "tha-ap2:5.4_0": "Vakkhāmahaṁ sādhu mahāmunīnaṁ.",
  "tha-ap2:6.1_0": "Sayameva buddhānaṁ mahāisīnaṁ,",
  "tha-ap2:6.2_0": "Sādhūni vākyāni madhūva khuddaṁ;",
  "tha-ap2:6.3_0": "Anuttaraṁ bhesajaṁ patthayantā,",
  "tha-ap2:6.4_0": "Suṇātha sabbesu pasannacittā’.",
  "tha-ap2:7.1_0": "Paccekabuddhānaṁ samāgatānaṁ,",
  "tha-ap2:7.2_0": "Paramparaṁ byākaraṇāni yāni;",
  "tha-ap2:7.3_0": "Ādīnavo yañca virāgavatthuṁ,",
  "tha-ap2:7.4_0": "Yathā ca bodhiṁ anupāpuṇiṁsu.",
  "tha-ap2:8.1_0": "Sarāgavatthūsu virāgasaññī,",
  "tha-ap2:8.2_0": "Rattamhi lokamhi virattacittā;",
  "tha-ap2:8.3_0": "Hitvā papañce jiya phanditāni,",
  "tha-ap2:8.4_0": "Tatheva bodhiṁ anupāpuṇiṁsu.",
  "tha-ap2:9.1_0": "Sabbesu bhūtesu nidhāya daṇḍaṁ,",
  "tha-ap2:9.2_0": "Aviheṭhayaṁ aññatarampi tesaṁ;",
  "tha-ap2:9.3_0": "Mettena cittena hitānukampī,",
  "tha-ap2:9.4_0": "Eko care khaggavisāṇakappo.",
  "tha-ap2:10.1_0": "Sabbesu bhūtesu nidhāya daṇḍaṁ,",
  "tha-ap2:10.2_0": "Aviheṭhayaṁ aññatarampi tesaṁ;",
  "tha-ap2:10.3_0": "Na puttamiccheyya kuto sahāyaṁ,",
  "tha-ap2:10.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:11.1_0": "‘Saṁsaggajātassa bhavanti snehā,",
  "tha-ap2:11.2_0": "Snehanvayaṁ dukkhamidaṁ pahoti;",
  "tha-ap2:11.3_0": "Ādīnavaṁ snehajaṁ pekkhamāno,",
  "tha-ap2:11.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:12.1_0": "‘Mitte suhajje anukampamāno,",
  "tha-ap2:12.2_0": "Hāpeti atthaṁ paṭibaddhacitto;",
  "tha-ap2:12.3_0": "Etaṁ bhayaṁ santhave pekkhamāno,",
  "tha-ap2:12.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:13.1_0": "‘Vaṁso visālova yathā visatto,",
  "tha-ap2:13.2_0": "Puttesu dāresu ca yā apekkhā;",
  "tha-ap2:13.3_0": "Vaṁse kaḷīrova asajjamāno,",
  "tha-ap2:13.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:14.1_0": "‘Migo araññamhi yathā abaddho,",
  "tha-ap2:14.2_0": "Yenicchakaṁ gacchati gocarāya;",
  "tha-ap2:14.3_0": "Viññū naro seritaṁ pekkhamāno,",
  "tha-ap2:14.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:15.1_0": "‘Āmantanā hoti sahāyamajjhe,",
  "tha-ap2:15.2_0": "Vāse ca ṭhāne gamane cārikāya;",
  "tha-ap2:15.3_0": "Anabhijjhitaṁ seritaṁ pekkhamāno,",
  "tha-ap2:15.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:16.1_0": "‘Khiḍḍā ratī hoti sahāyamajjhe,",
  "tha-ap2:16.2_0": "Puttesu pemaṁ vipulañca hoti;",
  "tha-ap2:16.3_0": "Piyavippayogaṁ vijigucchamāno,",
  "tha-ap2:16.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:17.1_0": "‘Cātuddiso appaṭigho ca hoti,",
  "tha-ap2:17.2_0": "Santussamāno itarītarena;",
  "tha-ap2:17.3_0": "Parissayānaṁ sahitā achambhī,",
  "tha-ap2:17.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:18.1_0": "‘Dussaṅgahā pabbajitāpi eke,",
  "tha-ap2:18.2_0": "Atho gahaṭṭhā gharamāvasantā;",
  "tha-ap2:18.3_0": "Appossukko paraputtesu hutvā,",
  "tha-ap2:18.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:19.1_0": "‘Oropayitvā gihibyañjanāni,",
  "tha-ap2:19.2_0": "Sañchinnapatto yathā koviḷāro;",
  "tha-ap2:19.3_0": "Chetvāna vīro gihibandhanāni,",
  "tha-ap2:19.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:20.1_0": "‘Sace labhetha nipakaṁ sahāyaṁ,",
  "tha-ap2:20.2_0": "Saddhiṁ caraṁ sādhuvihāridhīraṁ;",
  "tha-ap2:20.3_0": "Abhibhuyya sabbāni parissayāni,",
  "tha-ap2:20.4_0": "Careyya tenattamano satīmā’.",
  "tha-ap2:21.1_0": "‘No ce labhetha nipakaṁ sahāyaṁ,",
  "tha-ap2:21.2_0": "Saddhiṁ caraṁ sādhuvihāri dhīraṁ;",
  "tha-ap2:21.3_0": "Rājāva raṭṭhaṁ vijitaṁ pahāya,",
  "tha-ap2:21.4_0": "Eko care mātaṅgaraññeva nāgo’.",
  "tha-ap2:22.1_0": "‘Addhā pasaṁsāma sahāyasampadaṁ,",
  "tha-ap2:22.2_0": "Seṭṭhā samā sevitabbā sahāyā;",
  "tha-ap2:22.3_0": "Ete aladdhā anavajjabhojī,",
  "tha-ap2:22.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:23.1_0": "‘Disvā suvaṇṇassa pabhassarāni,",
  "tha-ap2:23.2_0": "Kammāraputtena suniṭṭhitāni;",
  "tha-ap2:23.3_0": "Saṅghaṭṭamānāni duve bhujasmiṁ,",
  "tha-ap2:23.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:24.1_0": "‘Evaṁ dutīyena sahā mamassa,",
  "tha-ap2:24.2_0": "Vācābhilāpo abhisajjanā vā;",
  "tha-ap2:24.3_0": "Etaṁ bhayaṁ āyatiṁ pekkhamāno,",
  "tha-ap2:24.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:25.1_0": "‘Kāmā hi citrā madhurā manoramā,",
  "tha-ap2:25.2_0": "Virūparūpena mathenti cittaṁ;",
  "tha-ap2:25.3_0": "Ādīnavaṁ kāmaguṇesu disvā,",
  "tha-ap2:25.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:26.1_0": "‘Ītī ca gaṇḍo ca upaddavo ca,",
  "tha-ap2:26.2_0": "Rogo ca sallañca bhayañca metaṁ;",
  "tha-ap2:26.3_0": "Etaṁ bhayaṁ kāmaguṇesu disvā,",
  "tha-ap2:26.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:27.1_0": "‘Sītañca uṇhañca khudaṁ pipāsaṁ,",
  "tha-ap2:27.2_0": "Vātātape ḍaṁsasarīsape ca;",
  "tha-ap2:27.3_0": "Sabbānipetāni abhibbhavitvā,",
  "tha-ap2:27.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:28.1_0": "‘Nāgova yūthāni vivajjayitvā,",
  "tha-ap2:28.2_0": "Sañjātakhandho padumī uḷāro;",
  "tha-ap2:28.3_0": "Yathābhirantaṁ viharaṁ araññe,",
  "tha-ap2:28.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:29.1_0": "‘Aṭṭhāna taṁ saṅgaṇikāratassa,",
  "tha-ap2:29.2_0": "Yaṁ phassaye sāmayikaṁ vimuttiṁ;",
  "tha-ap2:29.3_0": "Ādiccabandhussa vaco nisamma,",
  "tha-ap2:29.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:30.1_0": "‘Diṭṭhīvisūkāni upātivatto,",
  "tha-ap2:30.2_0": "Patto niyāmaṁ paṭiladdhamaggo;",
  "tha-ap2:30.3_0": "Uppannañāṇomhi anaññaneyyo,",
  "tha-ap2:30.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:31.1_0": "‘Nillolupo nikkuho nippipāso,",
  "tha-ap2:31.2_0": "Nimmakkha niddhantakasāvamoho;",
  "tha-ap2:31.3_0": "Nirāsayo sabbaloke bhavitvā,",
  "tha-ap2:31.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:32.1_0": "‘Pāpaṁ sahāyaṁ parivajjayetha,",
  "tha-ap2:32.2_0": "Anatthadassiṁ visame niviṭṭhaṁ;",
  "tha-ap2:32.3_0": "Sayaṁ na seve pasutaṁ pamattaṁ,",
  "tha-ap2:32.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:33.1_0": "‘Bahussutaṁ dhammadharaṁ bhajetha,",
  "tha-ap2:33.2_0": "Mittaṁ uḷāraṁ paṭibhānavantaṁ;",
  "tha-ap2:33.3_0": "Aññāya atthāni vineyya kaṅkhaṁ,",
  "tha-ap2:33.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:34.1_0": "‘Khiḍḍaṁ ratiṁ kāmasukhañca loke,",
  "tha-ap2:34.2_0": "Analaṅkaritvā anapekkhamāno;",
  "tha-ap2:34.3_0": "Vibhūsaṭṭhānā virato saccavādī,",
  "tha-ap2:34.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:35.1_0": "‘Puttañca dāraṁ pitarañca mātaraṁ,",
  "tha-ap2:35.2_0": "Dhanāni dhaññāni ca bandhavāni;",
  "tha-ap2:35.3_0": "Hitvāna kāmāni yathodhikāni,",
  "tha-ap2:35.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:36.1_0": "‘Saṅgo eso parittamettha sokhyaṁ,",
  "tha-ap2:36.2_0": "Appassādo dukkhamevettha bhiyyo;",
  "tha-ap2:36.3_0": "Gaḷo eso iti ñatvā matimā,",
  "tha-ap2:36.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:37.1_0": "‘Sandālayitvāna saṁyojanāni,",
  "tha-ap2:37.2_0": "Jālaṁva bhetvā salilambucārī;",
  "tha-ap2:37.3_0": "Aggīva daḍḍhaṁ anivattamāno,",
  "tha-ap2:37.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:38.1_0": "‘Okkhittacakkhū na ca pādalolo,",
  "tha-ap2:38.2_0": "Guttindriyo rakkhitamānasāno;",
  "tha-ap2:38.3_0": "Anavassuto apariḍayhamāno,",
  "tha-ap2:38.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:39.1_0": "‘Ohārayitvā gihibyañjanāni,",
  "tha-ap2:39.2_0": "Sañchannapatto yathā pārichatto;",
  "tha-ap2:39.3_0": "Kāsāyavattho abhinikkhamitvā,",
  "tha-ap2:39.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:40.1_0": "‘Rasesu gedhaṁ akaraṁ alolo,",
  "tha-ap2:40.2_0": "Anaññaposī sapadānacārī;",
  "tha-ap2:40.3_0": "Kule kule appaṭibaddhacitto,",
  "tha-ap2:40.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:41.1_0": "‘Pahāya pañcāvaraṇāni cetaso,",
  "tha-ap2:41.2_0": "Upakkilese byapanujja sabbe;",
  "tha-ap2:41.3_0": "Anissito chejja sinehadosaṁ,",
  "tha-ap2:41.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:42.1_0": "‘Vipiṭṭhikatvāna sukhañca dukkhaṁ,",
  "tha-ap2:42.2_0": "Pubbeva somanassadomanassaṁ;",
  "tha-ap2:42.3_0": "Laddhānupekkhaṁ samathaṁ visuddhaṁ,",
  "tha-ap2:42.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:43.1_0": "‘Āraddhavīriyo paramatthapattiyā,",
  "tha-ap2:43.2_0": "Alīnacitto akusītavutti;",
  "tha-ap2:43.3_0": "Daḷhanikkamo thāmabalūpapanno,",
  "tha-ap2:43.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:44.1_0": "‘Paṭisallānaṁ jhānamariñcamāno,",
  "tha-ap2:44.2_0": "Dhammesu niccaṁ anudhammacārī;",
  "tha-ap2:44.3_0": "Ādīnavaṁ sammasitā bhavesu,",
  "tha-ap2:44.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:45.1_0": "‘Taṇhakkhayaṁ patthayamappamatto,",
  "tha-ap2:45.2_0": "Aneḷamūgo sutavā satīmā;",
  "tha-ap2:45.3_0": "Saṅkhātadhammo niyato padhānavā,",
  "tha-ap2:45.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:46.1_0": "‘Sīhova saddesu asantasanto,",
  "tha-ap2:46.2_0": "Vātova jālamhi asajjamāno;",
  "tha-ap2:46.3_0": "Padumaṁva toyena alippamāno,",
  "tha-ap2:46.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:47.1_0": "‘Sīho yathā dāṭhabalī pasayha,",
  "tha-ap2:47.2_0": "Rājā migānaṁ abhibhuyya cārī;",
  "tha-ap2:47.3_0": "Sevetha pantāni senāsanāni,",
  "tha-ap2:47.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:48.1_0": "‘Mettaṁ upekkhaṁ karuṇaṁ vimuttiṁ,",
  "tha-ap2:48.2_0": "Āsevamāno muditañca kāle;",
  "tha-ap2:48.3_0": "Sabbena lokena avirujjhamāno,",
  "tha-ap2:48.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:49.1_0": "‘Rāgañca dosañca pahāya mohaṁ,",
  "tha-ap2:49.2_0": "Sandālayitvāna saṁyojanāni;",
  "tha-ap2:49.3_0": "Asantasaṁ jīvitasaṅkhayamhi,",
  "tha-ap2:49.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:50.1_0": "‘Bhajanti sevanti ca kāraṇatthā,",
  "tha-ap2:50.2_0": "Nikkāraṇā dullabhā ajja mittā;",
  "tha-ap2:50.3_0": "Attatthapaññā asucīmanussā,",
  "tha-ap2:50.4_0": "Eko care khaggavisāṇakappo’.",
  "tha-ap2:51.1_0": "Visuddhasīlā suvisuddhapaññā,",
  "tha-ap2:51.2_0": "Samāhitā jāgariyānuyuttā;",
  "tha-ap2:51.3_0": "Vipassakā dhammavisesadassī,",
  "tha-ap2:51.4_0": "Maggaṅgabojjhaṅgagate vijaññā.",
  "tha-ap2:52.1_0": "Suññappaṇidhiñca tathānimittaṁ,",
  "tha-ap2:52.2_0": "Āsevayitvā jinasāsanamhi;",
  "tha-ap2:52.3_0": "Ye sāvakattaṁ na vajanti dhīrā,",
  "tha-ap2:52.4_0": "Bhavanti paccekajinā sayambhū.",
  "tha-ap2:53.1_0": "Mahantadhammā bahudhammakāyā,",
  "tha-ap2:53.2_0": "Cittissarā sabbadukkhoghatiṇṇā;",
  "tha-ap2:53.3_0": "Udaggacittā paramatthadassī,",
  "tha-ap2:53.4_0": "Sīhopamā khaggavisāṇakappā.",
  "tha-ap2:54.1_0": "Santindriyā santamanā samādhī,",
  "tha-ap2:54.2_0": "Paccantasattesu patippacārā;",
  "tha-ap2:54.3_0": "Dīpā parattha idha vijjalantā,",
  "tha-ap2:54.4_0": "Paccekabuddhā satataṁ hitāme.",
  "tha-ap2:55.1_0": "Pahīnasabbāvaraṇā janindā,",
  "tha-ap2:55.2_0": "Lokappadīpā ghanakañcanābhā;",
  "tha-ap2:55.3_0": "Nissaṁsayaṁ lokasudakkhiṇeyyā,",
  "tha-ap2:55.4_0": "Paccekabuddhā satatappitāme.",
  "tha-ap2:56.1_0": "Paccekabuddhānaṁ subhāsitāni,",
  "tha-ap2:56.2_0": "Caranti lokamhi sadevakamhi;",
  "tha-ap2:56.3_0": "Sutvā tathā ye na karonti bālā,",
  "tha-ap2:56.4_0": "Caranti dukkhesu punappunaṁ te.",
  "tha-ap2:57.1_0": "Paccekabuddhāna subhāsitāni,",
  "tha-ap2:57.2_0": "Madhuṁ yathā khuddamavassavantaṁ;",
  "tha-ap2:57.3_0": "Sutvā tathā ye paṭipattiyuttā,",
  "tha-ap2:57.4_0": "Bhavanti te saccadasā sapaññā.",
  "tha-ap2:58.1_0": "Paccekabuddhehi jinehi bhāsitā,",
  "tha-ap2:58.2_0": "Kathā uḷārā abhinikkhamitvā;",
  "tha-ap2:58.3_0": "Tā sakyasīhena naruttamena,",
  "tha-ap2:58.4_0": "Pakāsitā dhammavijānanatthaṁ.",
  "tha-ap2:59.1_0": "Lokānukampāya imāni tesaṁ,",
  "tha-ap2:59.2_0": "Paccekabuddhāna vikubbitāni;",
  "tha-ap2:59.3_0": "Saṁvegasaṅgamativaḍḍhanatthaṁ,",
  "tha-ap2:59.4_0": "Sayambhusīhena pakāsitānī”ti.",
  "tha-ap2:60.1_0": "Paccekabuddhāpadānaṁ samattaṁ."
}