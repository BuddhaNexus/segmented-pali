{
  "ja101:0.1_0": "Jātaka",
  "ja101:0.2_0": "Ekakanipāta",
  "ja101:0.3_0": "Parosatavagga",
  "ja101:0.4_0": "1. Parosatajātaka",
  "ja101:1.1_0": "“Parosatañcepi samāgatānaṁ,",
  "ja101:1.2_0": "Jhāyeyyuṁ te vassasataṁ apaññā;",
  "ja101:1.3_0": "Ekova seyyo puriso sapañño,",
  "ja101:1.4_0": "Yo bhāsitassa vijānāti atthan”ti.",
  "ja101:2.1_0": "Parosatajātakaṁ paṭhamaṁ."
}