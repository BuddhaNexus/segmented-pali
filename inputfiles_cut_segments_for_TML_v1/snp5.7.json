{
  "snp5.7:0.1_0": "Sutta Nipāta 5.7",
  "snp5.7:0.2_0": "Upasīvamāṇavapucchā",
  "snp5.7:1.1_0": "“Eko ahaṁ sakka mahantamoghaṁ,",
  "snp5.7:1.2_0": "(iccāyasmā upasīvo)",
  "snp5.7:1.3_0": "Anissito no visahāmi tārituṁ;",
  "snp5.7:1.4_0": "Ārammaṇaṁ brūhi samantacakkhu,",
  "snp5.7:1.5_0": "Yaṁ nissito oghamimaṁ tareyyaṁ”.",
  "snp5.7:2.1_0": "“Ākiñcaññaṁ pekkhamāno satimā,",
  "snp5.7:2.2_0": "(upasīvāti bhagavā)",
  "snp5.7:2.3_0": "Natthīti nissāya tarassu oghaṁ;",
  "snp5.7:2.4_0": "Kāme pahāya virato kathāhi,",
  "snp5.7:2.5_0": "Taṇhakkhayaṁ nattamahābhipassa”.",
  "snp5.7:3.1_0": "“Sabbesu kāmesu yo vītarāgo,",
  "snp5.7:3.2_0": "(iccāyasmā upasīvo)",
  "snp5.7:3.3_0": "Ākiñcaññaṁ nissito hitvā maññaṁ;",
  "snp5.7:3.4_0": "Saññāvimokkhe parame vimutto,",
  "snp5.7:3.5_0": "Tiṭṭhe nu so tattha anānuyāyī”.",
  "snp5.7:4.1_0": "“Sabbesu kāmesu yo vītarāgo,",
  "snp5.7:4.2_0": "(upasīvāti bhagavā)",
  "snp5.7:4.3_0": "Ākiñcaññaṁ nissito hitvā maññaṁ;",
  "snp5.7:4.4_0": "Saññāvimokkhe parame vimutto,",
  "snp5.7:4.5_0": "Tiṭṭheyya so tattha anānuyāyī”.",
  "snp5.7:5.1_0": "“Tiṭṭhe ce so tattha anānuyāyī,",
  "snp5.7:5.2_0": "Pūgampi vassānaṁ samantacakkhu;",
  "snp5.7:5.3_0": "Tattheva so sītisiyā vimutto,",
  "snp5.7:5.4_0": "Cavetha viññāṇaṁ tathāvidhassa”.",
  "snp5.7:6.1_0": "“Accī yathā vātavegena khittā,",
  "snp5.7:6.2_0": "(upasīvāti bhagavā)",
  "snp5.7:6.3_0": "Atthaṁ paleti na upeti saṅkhaṁ;",
  "snp5.7:6.4_0": "Evaṁ munī nāmakāyā vimutto,",
  "snp5.7:6.5_0": "Atthaṁ paleti na upeti saṅkhaṁ”.",
  "snp5.7:7.1_0": "“Atthaṅgato so uda vā so natthi,",
  "snp5.7:7.2_0": "Udāhu ve sassatiyā arogo;",
  "snp5.7:7.3_0": "Taṁ me munī sādhu viyākarohi,",
  "snp5.7:7.4_0": "Tathā hi te vidito esa dhammo”.",
  "snp5.7:8.1_0": "“Atthaṅgatassa na pamāṇamatthi,",
  "snp5.7:8.2_0": "(upasīvāti bhagavā)",
  "snp5.7:8.3_0": "Yena naṁ vajjuṁ taṁ tassa natthi;",
  "snp5.7:8.4_0": "Sabbesu dhammesu samohatesu,",
  "snp5.7:8.5_0": "Samūhatā vādapathāpi sabbe”ti.",
  "snp5.7:8.6_0": "Upasīvamāṇavapucchā chaṭṭhī."
}