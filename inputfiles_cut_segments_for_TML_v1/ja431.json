{
  "ja431:0.1_0": "Jātaka",
  "ja431:0.2_0": "Navakanipāta",
  "ja431:0.3_0": "Gijjhavagga",
  "ja431:0.4_0": "5. Haritacajātaka",
  "ja431:1.1_0": "“Sutaṁ metaṁ mahābrahme,",
  "ja431:1.2_0": "kāme bhuñjati hārito;",
  "ja431:1.3_0": "Kaccetaṁ vacanaṁ tucchaṁ,",
  "ja431:1.4_0": "kacci suddho iriyyasi”.",
  "ja431:2.1_0": "“Evametaṁ mahārāja,",
  "ja431:2.2_0": "yathā te vacanaṁ sutaṁ;",
  "ja431:2.3_0": "Kummaggaṁ paṭipannosmi,",
  "ja431:2.4_0": "mohaneyyesu mucchito”.",
  "ja431:3.1_0": "“Adu paññā kimatthiyā,",
  "ja431:3.2_0": "nipuṇā sādhucintinī;",
  "ja431:3.3_0": "Yāya uppatitaṁ rāgaṁ,",
  "ja431:3.4_0": "kiṁ mano na vinodaye”.",
  "ja431:4.1_0": "“Cattārome mahārāja,",
  "ja431:4.2_0": "loke atibalā bhusā;",
  "ja431:4.3_0": "Rāgo doso mado moho,",
  "ja431:4.4_0": "yattha paññā na gādhati”.",
  "ja431:5.1_0": "“Arahā sīlasampanno,",
  "ja431:5.2_0": "suddho carati hārito;",
  "ja431:5.3_0": "Medhāvī paṇḍito ceva,",
  "ja431:5.4_0": "iti no sammato bhavaṁ”.",
  "ja431:6.1_0": "“Medhāvīnampi hiṁsanti,",
  "ja431:6.2_0": "isiṁ dhammaguṇe rataṁ;",
  "ja431:6.3_0": "Vitakkā pāpakā rāja,",
  "ja431:6.4_0": "subhā rāgūpasaṁhitā”.",
  "ja431:7.1_0": "“Uppannāyaṁ sarīrajo,",
  "ja431:7.2_0": "Rāgo vaṇṇavidūsano tava;",
  "ja431:7.3_0": "Taṁ pajaha bhaddamatthu te,",
  "ja431:7.4_0": "Bahunnāsi medhāvisammato”.",
  "ja431:8.1_0": "“Te andhakārake kāme,",
  "ja431:8.2_0": "Bahudukkhe mahāvise;",
  "ja431:8.3_0": "Tesaṁ mūlaṁ gavesissaṁ,",
  "ja431:8.4_0": "Checchaṁ rāgaṁ sabandhanaṁ”.",
  "ja431:9.1_0": "“Idaṁ vatvāna hārito,",
  "ja431:9.2_0": "isi saccaparakkamo;",
  "ja431:9.3_0": "Kāmarāgaṁ virājetvā,",
  "ja431:9.4_0": "brahmalokūpago ahū”ti.",
  "ja431:10.1_0": "Haritacajātakaṁ pañcamaṁ."
}