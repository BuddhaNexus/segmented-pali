{
  "tha-ap279:0.1_0": "Therāpadāna",
  "tha-ap279:0.2_0": "Suvaṇṇabibbohanavagga",
  "tha-ap279:0.3_0": "7. Citakapūjakattheraapadāna",
  "tha-ap279:1.1_0": "“Candabhāgānadītīre,",
  "tha-ap279:1.2_0": "anusotaṁ vajāmahaṁ;",
  "tha-ap279:1.3_0": "Satta māluvapupphāni,",
  "tha-ap279:1.4_0": "citamāropayiṁ ahaṁ.",
  "tha-ap279:2.1_0": "Catunnavutito kappe,",
  "tha-ap279:2.2_0": "citakaṁ yamapūjayiṁ;",
  "tha-ap279:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap279:2.4_0": "citapūjāyidaṁ phalaṁ.",
  "tha-ap279:3.1_0": "Sattasaṭṭhimhito kappe,",
  "tha-ap279:3.2_0": "paṭijaggasanāmakā;",
  "tha-ap279:3.3_0": "Sattaratanasampannā,",
  "tha-ap279:3.4_0": "sattāsuṁ cakkavattino.",
  "tha-ap279:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap279:4.2_0": "…pe…",
  "tha-ap279:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap279:5.1_0": "Itthaṁ sudaṁ āyasmā citakapūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap279:6.1_0": "Citakapūjakattherassāpadānaṁ sattamaṁ."
}