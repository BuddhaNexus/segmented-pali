{
  "thig1.11:0.1_0": "Therīgāthā",
  "thig1.11:0.2_0": "Ekakanipāta",
  "thig1.11:0.3_0": "11. Muttātherīgāthā",
  "thig1.11:1.1_0": "“Sumuttā sādhumuttāmhi,",
  "thig1.11:1.2_0": "tīhi khujjehi muttiyā;",
  "thig1.11:1.3_0": "Udukkhalena musalena,",
  "thig1.11:1.4_0": "patinā khujjakena ca;",
  "thig1.11:1.5_0": "Muttāmhi jātimaraṇā,",
  "thig1.11:1.6_0": "bhavanetti samūhatā”ti.",
  "thig1.11:2.1_0": "… Muttā therī …."
}