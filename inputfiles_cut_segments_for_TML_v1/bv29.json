{
  "bv29:0.1_0": "Buddhavaṁsa",
  "bv29:0.2_0": "29. Dhātubhājanīyakathā",
  "bv29:1.1_0": "Mahāgotamo jinavaro,",
  "bv29:1.2_0": "kusināramhi nibbuto;",
  "bv29:1.3_0": "Dhātuvitthārikaṁ āsi,",
  "bv29:1.4_0": "tesu tesu padesato.",
  "bv29:2.1_0": "Eko ajātasattussa,",
  "bv29:2.2_0": "eko vesāliyā pure;",
  "bv29:2.3_0": "Eko kapilavatthusmiṁ,",
  "bv29:2.4_0": "eko ca allakappake.",
  "bv29:3.1_0": "Eko ca rāmagāmamhi,",
  "bv29:3.2_0": "eko ca veṭhadīpake;",
  "bv29:3.3_0": "Eko pāveyyake malle,",
  "bv29:3.4_0": "eko ca kosinārake.",
  "bv29:4.1_0": "Kumbhassa thūpaṁ kāresi,",
  "bv29:4.2_0": "brāhmaṇo doṇasavhayo;",
  "bv29:4.3_0": "Aṅgārathūpaṁ kāresuṁ,",
  "bv29:4.4_0": "moriyā tuṭṭhamānasā.",
  "bv29:5.1_0": "Aṭṭha sārīrikā thūpā,",
  "bv29:5.2_0": "navamo kumbhacetiyo;",
  "bv29:5.3_0": "Aṅgārathūpo dasamo,",
  "bv29:5.4_0": "tadāyeva patiṭṭhito.",
  "bv29:6.1_0": "Uṇhīsaṁ catasso dāṭhā,",
  "bv29:6.2_0": "akkhakā dve ca dhātuyo;",
  "bv29:6.3_0": "Asambhinnā imā satta,",
  "bv29:6.4_0": "sesā bhinnāva dhātuyo.",
  "bv29:7.1_0": "Mahantā muggamattā ca,",
  "bv29:7.2_0": "majjhimā bhinnataṇḍulā;",
  "bv29:7.3_0": "Khuddakā sāsapamattā ca,",
  "bv29:7.4_0": "nānāvaṇṇā ca dhātuyo.",
  "bv29:8.1_0": "Mahantā suvaṇṇavaṇṇā ca,",
  "bv29:8.2_0": "muttavaṇṇā ca majjhimā;",
  "bv29:8.3_0": "Khuddakā makulavaṇṇā ca,",
  "bv29:8.4_0": "soḷasadoṇamattikā.",
  "bv29:9.1_0": "Mahantā pañca nāḷiyo,",
  "bv29:9.2_0": "nāḷiyo pañca majjhimā;",
  "bv29:9.3_0": "Khuddakā cha nāḷī ceva,",
  "bv29:9.4_0": "etā sabbāpi dhātuyo.",
  "bv29:10.1_0": "Uṇhīsaṁ sīhaḷe dīpe,",
  "bv29:10.2_0": "brahmaloke ca vāmakaṁ;",
  "bv29:10.3_0": "Sīhaḷe dakkhiṇakkhañca,",
  "bv29:10.4_0": "sabbāpetā patiṭṭhitā.",
  "bv29:11.1_0": "Ekā dāṭhā tidasapure,",
  "bv29:11.2_0": "ekā nāgapure ahu;",
  "bv29:11.3_0": "Ekā gandhāravisaye,",
  "bv29:11.4_0": "ekā kaliṅgarājino.",
  "bv29:12.1_0": "Cattālīsasamā dantā,",
  "bv29:12.2_0": "kesā lomā ca sabbaso;",
  "bv29:12.3_0": "Devā hariṁsu ekekaṁ,",
  "bv29:12.4_0": "cakkavāḷaparamparā.",
  "bv29:13.1_0": "Vajirāyaṁ bhagavato,",
  "bv29:13.2_0": "Patto daṇḍañca cīvaraṁ;",
  "bv29:13.3_0": "Nivāsanaṁ kulaghare,",
  "bv29:13.4_0": "Paccattharaṇaṁ kapilavhaye.",
  "bv29:14.1_0": "Pāṭaliputtapuramhi,",
  "bv29:14.2_0": "karaṇaṁ kāyabandhanaṁ;",
  "bv29:14.3_0": "Campāyudakasāṭiyaṁ,",
  "bv29:14.4_0": "uṇṇalomañca kosale.",
  "bv29:15.1_0": "Kāsāvaṁ brahmaloke ca,",
  "bv29:15.2_0": "veṭhanaṁ tidase pure;",
  "bv29:15.3_0": "Nisīdanaṁ avantīsu,",
  "bv29:15.4_0": "raṭṭhe attharaṇaṁ tadā.",
  "bv29:16.1_0": "Araṇī ca mithilāyaṁ,",
  "bv29:16.2_0": "videhe parisāvanaṁ;",
  "bv29:16.3_0": "Vāsi sūcigharañcāpi,",
  "bv29:16.4_0": "indapatthapure tadā.",
  "bv29:17.1_0": "Parikkhārā avasesā,",
  "bv29:17.2_0": "janapade aparantake;",
  "bv29:17.3_0": "Paribhuttāni muninā,",
  "bv29:17.4_0": "akaṁsu manujā tadā.",
  "bv29:18.1_0": "Dhātuvitthārikaṁ āsi,",
  "bv29:18.2_0": "Gotamassa mahesino;",
  "bv29:18.3_0": "Pāṇīnaṁ anukampāya,",
  "bv29:18.4_0": "Ahu porāṇikaṁ tadāti. "
}