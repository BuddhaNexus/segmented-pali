{
  "vv44:0.1_0": "Vimānavatthu",
  "vv44:0.2_0": "Itthivimāna",
  "vv44:0.3_0": "Mañjiṭṭhakavagga",
  "vv44:0.4_0": "6. Vihāravimānavatthu",
  "vv44:1.1_0": "“Abhikkantena vaṇṇena,",
  "vv44:1.2_0": "…pe…",
  "vv44:1.3_0": "osadhī viya tārakā.",
  "vv44:2.1_0": "Tassā te naccamānāya,",
  "vv44:2.2_0": "aṅgamaṅgehi sabbaso;",
  "vv44:2.3_0": "Dibbā saddā niccharanti,",
  "vv44:2.4_0": "savanīyā manoramā.",
  "vv44:3.1_0": "Tassā te naccamānāya,",
  "vv44:3.2_0": "aṅgamaṅgehi sabbaso;",
  "vv44:3.3_0": "Dibbā gandhā pavāyanti,",
  "vv44:3.4_0": "sucigandhā manoramā.",
  "vv44:4.1_0": "Vivattamānā kāyena,",
  "vv44:4.2_0": "yā veṇīsu piḷandhanā;",
  "vv44:4.3_0": "Tesaṁ suyyati nigghoso,",
  "vv44:4.4_0": "tūriye pañcaṅgike yathā.",
  "vv44:5.1_0": "Vaṭaṁsakā vātadhutā,",
  "vv44:5.2_0": "vātena sampakampitā;",
  "vv44:5.3_0": "Tesaṁ suyyati nigghoso,",
  "vv44:5.4_0": "tūriye pañcaṅgike yathā.",
  "vv44:6.1_0": "Yāpi te sirasmiṁ mālā,",
  "vv44:6.2_0": "sucigandhā manoramā;",
  "vv44:6.3_0": "Vāti gandho disā sabbā,",
  "vv44:6.4_0": "rukkho mañjūsako yathā.",
  "vv44:7.1_0": "Ghāyase taṁ sucigandhaṁ,",
  "vv44:7.2_0": "rūpaṁ passasi amānusaṁ;",
  "vv44:7.3_0": "Devate pucchitācikkha,",
  "vv44:7.4_0": "kissa kammassidaṁ phalan”ti.",
  "vv44:8.1_0": "“Sāvatthiyaṁ mayhaṁ sakhī bhadante,",
  "vv44:8.2_0": "Saṅghassa kāresi mahāvihāraṁ;",
  "vv44:8.3_0": "Tatthappasannā ahamānumodiṁ,",
  "vv44:8.4_0": "Disvā agārañca piyañca metaṁ.",
  "vv44:9.1_0": "Tāyeva me suddhanumodanāya,",
  "vv44:9.2_0": "Laddhaṁ vimānabbhutadassaneyyaṁ;",
  "vv44:9.3_0": "Samantato soḷasayojanāni,",
  "vv44:9.4_0": "Vehāyasaṁ gacchati iddhiyā mama.",
  "vv44:10.1_0": "Kūṭāgārā nivesā me,",
  "vv44:10.2_0": "vibhattā bhāgaso mitā;",
  "vv44:10.3_0": "Daddallamānā ābhanti,",
  "vv44:10.4_0": "samantā satayojanaṁ.",
  "vv44:11.1_0": "Pokkharañño ca me ettha,",
  "vv44:11.2_0": "puthulomanisevitā;",
  "vv44:11.3_0": "Acchodakā vippasannā,",
  "vv44:11.4_0": "soṇṇavālukasanthatā.",
  "vv44:12.1_0": "Nānāpadumasañchannā,",
  "vv44:12.2_0": "puṇḍarīkasamotatā;",
  "vv44:12.3_0": "Surabhī sampavāyanti,",
  "vv44:12.4_0": "manuññā māluteritā.",
  "vv44:13.1_0": "Jambuyo panasā tālā,",
  "vv44:13.2_0": "nāḷikeravanāni ca;",
  "vv44:13.3_0": "Antonivesane jātā,",
  "vv44:13.4_0": "nānārukkhā aropimā.",
  "vv44:14.1_0": "Nānātūriyasaṅghuṭṭhaṁ,",
  "vv44:14.2_0": "accharāgaṇaghositaṁ;",
  "vv44:14.3_0": "Yopi maṁ supine passe,",
  "vv44:14.4_0": "sopi vitto siyā naro.",
  "vv44:15.1_0": "Etādisaṁ abbhutadassaneyyaṁ,",
  "vv44:15.2_0": "Vimānaṁ sabbasopabhaṁ;",
  "vv44:15.3_0": "Mama kammehi nibbattaṁ,",
  "vv44:15.4_0": "Alaṁ puññāni kātave”ti.",
  "vv44:16.1_0": "“Tāyeva te suddhanumodanāya,",
  "vv44:16.2_0": "Laddhaṁ vimānabbhutadassaneyyaṁ;",
  "vv44:16.3_0": "Yā ceva sā dānamadāsi nārī,",
  "vv44:16.4_0": "Tassā gatiṁ brūhi kuhiṁ uppannā sā”ti.",
  "vv44:17.1_0": "“Yā sā ahu mayhaṁ sakhī bhadante,",
  "vv44:17.2_0": "Saṅghassa kāresi mahāvihāraṁ;",
  "vv44:17.3_0": "Viññātadhammā sā adāsi dānaṁ,",
  "vv44:17.4_0": "Uppannā nimmānaratīsu devesu.",
  "vv44:18.1_0": "Pajāpatī tassa sunimmitassa,",
  "vv44:18.2_0": "Acintiyā kammavipākā tassā;",
  "vv44:18.3_0": "Yametaṁ pucchasi kuhiṁ uppannā sāti,",
  "vv44:18.4_0": "Taṁ te viyākāsiṁ anaññathā ahaṁ.",
  "vv44:19.1_0": "Tenahaññepi samādapetha,",
  "vv44:19.2_0": "Saṅghassa dānāni dadātha vittā;",
  "vv44:19.3_0": "Dhammañca suṇātha pasannamānasā,",
  "vv44:19.4_0": "Sudullabho laddho manussalābho.",
  "vv44:20.1_0": "Yaṁ maggaṁ maggādhipatī adesayi,",
  "vv44:20.2_0": "Brahmassaro kañcanasannibhattaco;",
  "vv44:20.3_0": "Saṅghassa dānāni dadātha vittā,",
  "vv44:20.4_0": "Mahapphalā yattha bhavanti dakkhiṇā.",
  "vv44:21.1_0": "Ye puggalā aṭṭha sataṁ pasatthā,",
  "vv44:21.2_0": "Cattāri etāni yugāni honti;",
  "vv44:21.3_0": "Te dakkhiṇeyyā sugatassa sāvakā,",
  "vv44:21.4_0": "Etesu dinnāni mahapphalāni.",
  "vv44:22.1_0": "Cattāro ca paṭipannā,",
  "vv44:22.2_0": "cattāro ca phale ṭhitā;",
  "vv44:22.3_0": "Esa saṅgho ujubhūto,",
  "vv44:22.4_0": "paññāsīlasamāhito.",
  "vv44:23.1_0": "Yajamānānaṁ manussānaṁ,",
  "vv44:23.2_0": "puññapekkhāna pāṇinaṁ;",
  "vv44:23.3_0": "Karotaṁ opadhikaṁ puññaṁ,",
  "vv44:23.4_0": "saṅghe dinnaṁ mahapphalaṁ.",
  "vv44:24.1_0": "Eso hi saṅgho vipulo mahaggato,",
  "vv44:24.2_0": "Esappameyyo udadhīva sāgaro;",
  "vv44:24.3_0": "Etehi seṭṭhā naravīrasāvakā,",
  "vv44:24.4_0": "Pabhaṅkarā dhammamudīrayanti.",
  "vv44:25.1_0": "Tesaṁ sudinnaṁ suhutaṁ suyiṭṭhaṁ,",
  "vv44:25.2_0": "Ye saṅghamuddissa dadanti dānaṁ;",
  "vv44:25.3_0": "Sā dakkhiṇā saṅghagatā patiṭṭhitā,",
  "vv44:25.4_0": "Mahapphalā lokavidūna vaṇṇitā.",
  "vv44:26.1_0": "Etādisaṁ yaññamanussarantā,",
  "vv44:26.2_0": "Ye vedajātā vicaranti loke;",
  "vv44:26.3_0": "Vineyya maccheramalaṁ samūlaṁ,",
  "vv44:26.4_0": "Aninditā saggamupenti ṭhānan”ti.",
  "vv44:27.1_0": "Vihāravimānaṁ chaṭṭhaṁ.",
  "vv44:27.2_0": "Bhāṇavāraṁ dutiyaṁ."
}