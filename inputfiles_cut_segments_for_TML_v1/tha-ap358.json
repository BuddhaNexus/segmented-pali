{
  "tha-ap358:0.1_0": "Therāpadāna",
  "tha-ap358:0.2_0": "Saddasaññakavagga",
  "tha-ap358:0.3_0": "6. Ambayāgudāyakattheraapadāna",
  "tha-ap358:1.1_0": "“Sataraṁsī nāma sambuddho,",
  "tha-ap358:1.2_0": "sayambhū aparājito;",
  "tha-ap358:1.3_0": "Vuṭṭhahitvā samādhimhā,",
  "tha-ap358:1.4_0": "bhikkhāya mamupāgami.",
  "tha-ap358:2.1_0": "Paccekabuddhaṁ disvāna,",
  "tha-ap358:2.2_0": "ambayāguṁ adāsahaṁ;",
  "tha-ap358:2.3_0": "Vippasannamanaṁ tassa,",
  "tha-ap358:2.4_0": "vippasannena cetasā.",
  "tha-ap358:3.1_0": "Catunnavutito kappe,",
  "tha-ap358:3.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap358:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap358:3.4_0": "ambayāguyidaṁ phalaṁ.",
  "tha-ap358:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap358:4.2_0": "…pe…",
  "tha-ap358:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap358:5.1_0": "Itthaṁ sudaṁ āyasmā ambayāgudāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap358:6.1_0": "Ambayāgudāyakattherassāpadānaṁ chaṭṭhaṁ."
}