{
  "tha-ap365:0.1_0": "Therāpadāna",
  "tha-ap365:0.2_0": "Mandāravapupphiyavagga",
  "tha-ap365:0.3_0": "3. Bhisamuḷāladāyakattheraapadāna",
  "tha-ap365:1.1_0": "“Phusso nāmāsi sambuddho,",
  "tha-ap365:1.2_0": "sabbadhammāna pāragū;",
  "tha-ap365:1.3_0": "Vivekakāmo sabbaññū,",
  "tha-ap365:1.4_0": "āgañchi mama santike.",
  "tha-ap365:2.1_0": "Tasmiṁ cittaṁ pasādetvā,",
  "tha-ap365:2.2_0": "mahākāruṇike jine;",
  "tha-ap365:2.3_0": "Bhisamuḷālaṁ paggayha,",
  "tha-ap365:2.4_0": "buddhaseṭṭhassadāsahaṁ.",
  "tha-ap365:3.1_0": "Dvenavute ito kappe,",
  "tha-ap365:3.2_0": "yaṁ bhisamadadiṁ tadā;",
  "tha-ap365:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap365:3.4_0": "bhisadānassidaṁ phalaṁ.",
  "tha-ap365:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap365:4.2_0": "…pe…",
  "tha-ap365:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap365:5.1_0": "Itthaṁ sudaṁ āyasmā bhisamuḷāladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap365:6.1_0": "Bhisamuḷāladāyakattherassāpadānaṁ tatiyaṁ."
}