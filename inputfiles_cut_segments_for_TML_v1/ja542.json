{
  "ja542:0.1_0": "Jātaka",
  "ja542:0.2_0": "Mahānipāta",
  "ja542:0.3_0": "Mūgapakkhavagga",
  "ja542:0.4_0": "5 Umaṅgajātaka",
  "ja542:1.1_0": "“Pañcālo sabbasenāya,",
  "ja542:1.2_0": "brahmadattoyamāgato;",
  "ja542:1.3_0": "Sāyaṁ pañcāliyā senā,",
  "ja542:1.4_0": "appameyyā mahosadha.",
  "ja542:2.1_0": "Vīthimatī pattimatī,",
  "ja542:2.2_0": "sabbasaṅgāmakovidā;",
  "ja542:2.3_0": "Ohārinī saddavatī,",
  "ja542:2.4_0": "bherisaṅkhappabodhanā.",
  "ja542:3.1_0": "Lohavijjālaṅkārābhā,",
  "ja542:3.2_0": "dhajinī vāmarohinī;",
  "ja542:3.3_0": "Sippiyehi susampannā,",
  "ja542:3.4_0": "sūrehi suppatiṭṭhitā.",
  "ja542:4.1_0": "Dasettha paṇḍitā āhu,",
  "ja542:4.2_0": "bhūripaññā rahogamā;",
  "ja542:4.3_0": "Mātā ekādasī rañño,",
  "ja542:4.4_0": "pañcāliyaṁ pasāsati.",
  "ja542:5.1_0": "Athetthekasataṁ khatyā,",
  "ja542:5.2_0": "anuyantā yasassino;",
  "ja542:5.3_0": "Acchinnaraṭṭhā byathitā,",
  "ja542:5.4_0": "pañcāliyaṁ vasaṁ gatā.",
  "ja542:6.1_0": "Yaṁvadā takkarā rañño,",
  "ja542:6.2_0": "akāmā piyabhāṇino;",
  "ja542:6.3_0": "Pañcālamanuyāyanti,",
  "ja542:6.4_0": "akāmā vasino gatā.",
  "ja542:7.1_0": "Tāya senāya mithilā,",
  "ja542:7.2_0": "tisandhiparivāritā;",
  "ja542:7.3_0": "Rājadhānī videhānaṁ,",
  "ja542:7.4_0": "samantā parikhaññati.",
  "ja542:8.1_0": "Uddhaṁ tārakajātāva,",
  "ja542:8.2_0": "samantā parivāritā;",
  "ja542:8.3_0": "Mahosadha vijānāhi,",
  "ja542:8.4_0": "kathaṁ mokkho bhavissati”.",
  "ja542:9.1_0": "“Pāde deva pasārehi,",
  "ja542:9.2_0": "bhuñja kāme ramassu ca;",
  "ja542:9.3_0": "Hitvā pañcāliyaṁ senaṁ,",
  "ja542:9.4_0": "brahmadatto palāyiti”.",
  "ja542:10.1_0": "“Rājā santhavakāmo te,",
  "ja542:10.2_0": "ratanāni pavecchati;",
  "ja542:10.3_0": "Āgacchantu ito dūtā,",
  "ja542:10.4_0": "mañjukā piyabhāṇino.",
  "ja542:11.1_0": "Bhāsantu mudukā vācā,",
  "ja542:11.2_0": "yā vācā paṭinanditā;",
  "ja542:11.3_0": "Pañcālo ca videho ca,",
  "ja542:11.4_0": "ubho ekā bhavantu te”.",
  "ja542:12.1_0": "“Kathaṁ nu kevaṭṭa mahosadhena,",
  "ja542:12.2_0": "Samāgamo āsi tadiṅgha brūhi;",
  "ja542:12.3_0": "Kacci te paṭinijjhatto,",
  "ja542:12.4_0": "Kacci tuṭṭho mahosadho”.",
  "ja542:13.1_0": "“Anariyarūpo puriso janinda,",
  "ja542:13.2_0": "Asammodako thaddho asabbhirūpo;",
  "ja542:13.3_0": "Yathā mūgo ca badhiro ca,",
  "ja542:13.4_0": "Na kiñcitthaṁ abhāsatha”.",
  "ja542:14.1_0": "“Addhā idaṁ mantapadaṁ sududdasaṁ,",
  "ja542:14.2_0": "Attho suddho naraviriyena diṭṭho;",
  "ja542:14.3_0": "Tathā hi kāyo mama sampavedhati,",
  "ja542:14.4_0": "Hitvā sayaṁ ko parahatthamessati”.",
  "ja542:15.1_0": "“Channañhi ekāva matī sameti,",
  "ja542:15.2_0": "Ye paṇḍitā uttamabhūripattā;",
  "ja542:15.3_0": "Yānaṁ ayānaṁ atha vāpi ṭhānaṁ,",
  "ja542:15.4_0": "Mahosadha tvampi matiṁ karohi”.",
  "ja542:16.1_0": "“Jānāsi kho rāja mahānubhāvo,",
  "ja542:16.2_0": "Mahabbalo cūḷanibrahmadatto;",
  "ja542:16.3_0": "Rājā ca taṁ icchati māraṇatthaṁ,",
  "ja542:16.4_0": "Migaṁ yathā okacarena luddo.",
  "ja542:17.1_0": "Yathāpi maccho baḷisaṁ,",
  "ja542:17.2_0": "vaṅkaṁ maṁsena chāditaṁ;",
  "ja542:17.3_0": "Āmagiddho na jānāti,",
  "ja542:17.4_0": "maccho maraṇamattano.",
  "ja542:18.1_0": "Evameva tuvaṁ rāja,",
  "ja542:18.2_0": "cūḷaneyyassa dhītaraṁ;",
  "ja542:18.3_0": "Kāmagiddho na jānāsi,",
  "ja542:18.4_0": "macchova maraṇamattano.",
  "ja542:19.1_0": "Sace gacchasi pañcālaṁ,",
  "ja542:19.2_0": "Khippamattaṁ jahissasi;",
  "ja542:19.3_0": "Migaṁ panthānubandhaṁva,",
  "ja542:19.4_0": "Mahantaṁ bhayamessati”.",
  "ja542:20.1_0": "“Mayameva bālamhase eḷamūgā,",
  "ja542:20.2_0": "Ye uttamatthāni tayī lapimhā;",
  "ja542:20.3_0": "Kimeva tvaṁ naṅgalakoṭivaḍḍho,",
  "ja542:20.4_0": "Atthāni jānāsi yathāpi aññe”.",
  "ja542:21.1_0": "“Imaṁ gale gahetvāna,",
  "ja542:21.2_0": "nāsetha vijitā mama;",
  "ja542:21.3_0": "Yo me ratanalābhassa,",
  "ja542:21.4_0": "antarāyāya bhāsati”.",
  "ja542:22.1_0": "Tato ca so apakkamma,",
  "ja542:22.2_0": "vedehassa upantikā;",
  "ja542:22.3_0": "Atha āmantayī dūtaṁ,",
  "ja542:22.4_0": "mādharaṁ suvapaṇḍitaṁ.",
  "ja542:23.1_0": "“Ehi samma haritapakkha,",
  "ja542:23.2_0": "veyyāvaccaṁ karohi me;",
  "ja542:23.3_0": "Atthi pañcālarājassa,",
  "ja542:23.4_0": "sāḷikā sayanapālikā.",
  "ja542:24.1_0": "Taṁ bandhanena pucchassu,",
  "ja542:24.2_0": "sā hi sabbassa kovidā;",
  "ja542:24.3_0": "Sā tesaṁ sabbaṁ jānāti,",
  "ja542:24.4_0": "rañño ca kosiyassa ca”.",
  "ja542:25.1_0": "“Āmo”ti so paṭissutvā,",
  "ja542:25.2_0": "mādharo suvapaṇḍito;",
  "ja542:25.3_0": "Agamāsi haritapakkho,",
  "ja542:25.4_0": "sāḷikāya upantikaṁ.",
  "ja542:26.1_0": "Tato ca kho so gantvāna,",
  "ja542:26.2_0": "mādharo suvapaṇḍito;",
  "ja542:26.3_0": "Athāmantayi sugharaṁ,",
  "ja542:26.4_0": "sāḷikaṁ mañjubhāṇikaṁ.",
  "ja542:27.1_0": "“Kacci te sughare khamanīyaṁ,",
  "ja542:27.2_0": "Kacci vesse anāmayaṁ;",
  "ja542:27.3_0": "Kacci te madhunā lājā,",
  "ja542:27.4_0": "Labbhate sughare tuvaṁ”.",
  "ja542:28.1_0": "“Kusalañceva me samma,",
  "ja542:28.2_0": "atho samma anāmayaṁ;",
  "ja542:28.3_0": "Atho me madhunā lājā,",
  "ja542:28.4_0": "labbhate suvapaṇḍita.",
  "ja542:29.1_0": "Kuto nu samma āgamma,",
  "ja542:29.2_0": "kassa vā pahito tuvaṁ;",
  "ja542:29.3_0": "Na ca mesi ito pubbe,",
  "ja542:29.4_0": "diṭṭho vā yadi vā suto”.",
  "ja542:30.1_0": "“Ahosiṁ sivirājassa,",
  "ja542:30.2_0": "pāsāde sayanapālako;",
  "ja542:30.3_0": "Tato so dhammiko rājā,",
  "ja542:30.4_0": "baddhe mocesi bandhanā.",
  "ja542:31.1_0": "Tassa mekā dutiyāsi,",
  "ja542:31.2_0": "sāḷikā mañjubhāṇikā;",
  "ja542:31.3_0": "Taṁ tattha avadhī seno,",
  "ja542:31.4_0": "pekkhato sughare mama.",
  "ja542:32.1_0": "Tassā kāmā hi sammatto,",
  "ja542:32.2_0": "āgatosmi tavantike;",
  "ja542:32.3_0": "Sace kareyya okāsaṁ,",
  "ja542:32.4_0": "ubhayova vasāmase”.",
  "ja542:33.1_0": "“Suvova suviṁ kāmeyya,",
  "ja542:33.2_0": "sāḷiko pana sāḷikaṁ;",
  "ja542:33.3_0": "Suvassa sāḷikāyeva,",
  "ja542:33.4_0": "saṁvāso hoti kīdiso”.",
  "ja542:34.1_0": "“Yoyaṁ kāme kāmayati,",
  "ja542:34.2_0": "api caṇḍālikāmapi;",
  "ja542:34.3_0": "Sabbo hi sadiso hoti,",
  "ja542:34.4_0": "natthi kāme asādiso”.",
  "ja542:35.1_0": "“Atthi jampāvatī nāma,",
  "ja542:35.2_0": "mātā sivissa rājino;",
  "ja542:35.3_0": "Sā bhariyā vāsudevassa,",
  "ja542:35.4_0": "kaṇhassa mahesī piyā.",
  "ja542:36.1_0": "Raṭṭhavatī kimpurisī,",
  "ja542:36.2_0": "sāpi vacchaṁ akāmayi;",
  "ja542:36.3_0": "Manusso migiyā saddhiṁ,",
  "ja542:36.4_0": "natthi kāme asādiso.",
  "ja542:37.1_0": "Handa khvāhaṁ gamissāmi,",
  "ja542:37.2_0": "sāḷike mañjubhāṇike;",
  "ja542:37.3_0": "Paccakkhānupadañhetaṁ,",
  "ja542:37.4_0": "atimaññasi nūna maṁ”.",
  "ja542:38.1_0": "“Na sirī taramānassa,",
  "ja542:38.2_0": "mādhara suvapaṇḍita;",
  "ja542:38.3_0": "Idheva tāva acchassu,",
  "ja542:38.4_0": "yāva rājāna dakkhasi;",
  "ja542:38.5_0": "Sossi saddaṁ mudiṅgānaṁ,",
  "ja542:38.6_0": "ānubhāvañca rājino”.",
  "ja542:39.1_0": "“Yo nu khvāyaṁ tibbo saddo,",
  "ja542:39.2_0": "tirojanapade suto;",
  "ja542:39.3_0": "Dhītā pañcālarājassa,",
  "ja542:39.4_0": "osadhī viya vaṇṇinī;",
  "ja542:39.5_0": "Taṁ dassati videhānaṁ,",
  "ja542:39.6_0": "so vivāho bhavissati”.",
  "ja542:40.1_0": "“Ediso mā amittānaṁ,",
  "ja542:40.2_0": "vivāho hotu mādhara;",
  "ja542:40.3_0": "Yathā pañcālarājassa,",
  "ja542:40.4_0": "vedehena bhavissati.",
  "ja542:41.1_0": "Ānayitvāna vedehaṁ,",
  "ja542:41.2_0": "pañcālānaṁ rathesabho;",
  "ja542:41.3_0": "Tato naṁ ghātayissati,",
  "ja542:41.4_0": "nassa sakhī bhavissati”.",
  "ja542:42.1_0": "“Handa kho maṁ anujānāhi,",
  "ja542:42.2_0": "rattiyo sattamattiyo;",
  "ja542:42.3_0": "Yāvāhaṁ sivirājassa,",
  "ja542:42.4_0": "ārocemi mahesino;",
  "ja542:42.5_0": "Laddho ca me āvasatho,",
  "ja542:42.6_0": "sāḷikāya upantikaṁ”.",
  "ja542:43.1_0": "“Handa kho taṁ anujānāmi,",
  "ja542:43.2_0": "rattiyo sattamattiyo;",
  "ja542:43.3_0": "Sace tvaṁ sattarattena,",
  "ja542:43.4_0": "nāgacchasi mamantike;",
  "ja542:43.5_0": "Maññe okkantasattaṁ maṁ,",
  "ja542:43.6_0": "matāya āgamissasi”.",
  "ja542:44.1_0": "Tato ca kho so gantvāna,",
  "ja542:44.2_0": "mādharo suvapaṇḍito;",
  "ja542:44.3_0": "Mahosadhassa akkhāsi,",
  "ja542:44.4_0": "sāḷikāvacanaṁ idaṁ.",
  "ja542:45.1_0": "“Yasseva ghare bhuñjeyya bhogaṁ,",
  "ja542:45.2_0": "Tasseva atthaṁ puriso careyya;",
  "ja542:45.3_0": "Handāhaṁ gacchāmi pure janinda,",
  "ja542:45.4_0": "Pañcālarājassa puraṁ surammaṁ;",
  "ja542:45.5_0": "Nivesanāni māpetuṁ,",
  "ja542:45.6_0": "Vedehassa yasassino.",
  "ja542:46.1_0": "Nivesanāni māpetvā,",
  "ja542:46.2_0": "vedehassa yasassino;",
  "ja542:46.3_0": "Yadā te pahiṇeyyāmi,",
  "ja542:46.4_0": "tadā eyyāsi khattiya”.",
  "ja542:47.1_0": "Tato ca pāyāsi pure mahosadho,",
  "ja542:47.2_0": "Pañcālarājassa puraṁ surammaṁ;",
  "ja542:47.3_0": "Nivesanāni māpetuṁ,",
  "ja542:47.4_0": "Vedehassa yasassino.",
  "ja542:48.1_0": "Nivesanāni māpetvā,",
  "ja542:48.2_0": "vedehassa yasassino;",
  "ja542:48.3_0": "Athassa pāhiṇī dūtaṁ,",
  "ja542:48.4_0": "vedehaṁ mithilaggahaṁ;",
  "ja542:48.5_0": "“Ehi dāni mahārāja,",
  "ja542:48.6_0": "māpitaṁ te nivesanaṁ”.",
  "ja542:49.1_0": "Tato ca rājā pāyāsi,",
  "ja542:49.2_0": "senāya caturaṅgiyā;",
  "ja542:49.3_0": "Anantavāhanaṁ daṭṭhuṁ,",
  "ja542:49.4_0": "phītaṁ kapiliyaṁ puraṁ.",
  "ja542:50.1_0": "Tato ca kho so gantvāna,",
  "ja542:50.2_0": "brahmadattassa pāhiṇi;",
  "ja542:50.3_0": "“Āgatosmi mahārāja,",
  "ja542:50.4_0": "tava pādāni vandituṁ.",
  "ja542:51.1_0": "Dadāhi dāni me bhariyaṁ,",
  "ja542:51.2_0": "nāriṁ sabbaṅgasobhiniṁ;",
  "ja542:51.3_0": "Suvaṇṇena paṭicchannaṁ,",
  "ja542:51.4_0": "dāsīgaṇapurakkhataṁ”.",
  "ja542:52.1_0": "“Svāgataṁ teva vedeha,",
  "ja542:52.2_0": "atho te adurāgataṁ;",
  "ja542:52.3_0": "Nakkhattaṁyeva paripuccha,",
  "ja542:52.4_0": "ahaṁ kaññaṁ dadāmi te;",
  "ja542:52.5_0": "Suvaṇṇena paṭicchannaṁ,",
  "ja542:52.6_0": "dāsīgaṇapurakkhataṁ”.",
  "ja542:53.1_0": "Tato ca rājā vedeho,",
  "ja542:53.2_0": "nakkhattaṁ paripucchatha;",
  "ja542:53.3_0": "Nakkhattaṁ paripucchitvā,",
  "ja542:53.4_0": "brahmadattassa pāhiṇi.",
  "ja542:54.1_0": "“Dadāhi dāni me bhariyaṁ,",
  "ja542:54.2_0": "nāriṁ sabbaṅgasobhiniṁ;",
  "ja542:54.3_0": "Suvaṇṇena paṭicchannaṁ,",
  "ja542:54.4_0": "dāsīgaṇapurakkhataṁ”.",
  "ja542:55.1_0": "“Dadāmi dāni te bhariyaṁ,",
  "ja542:55.2_0": "nāriṁ sabbaṅgasobhiniṁ;",
  "ja542:55.3_0": "Suvaṇṇena paṭicchannaṁ,",
  "ja542:55.4_0": "dāsīgaṇapurakkhataṁ”.",
  "ja542:56.1_0": "“Hatthī assā rathā pattī,",
  "ja542:56.2_0": "senā tiṭṭhanti vammitā;",
  "ja542:56.3_0": "Ukkā padittā jhāyanti,",
  "ja542:56.4_0": "kiṁ nu maññanti paṇḍitā.",
  "ja542:57.1_0": "Hatthī assā rathā pattī,",
  "ja542:57.2_0": "senā tiṭṭhanti vammitā;",
  "ja542:57.3_0": "Ukkā padittā jhāyanti,",
  "ja542:57.4_0": "kiṁ nu kāhanti paṇḍita”.",
  "ja542:58.1_0": "“Rakkhati taṁ mahārāja,",
  "ja542:58.2_0": "cūḷaneyyo mahabbalo;",
  "ja542:58.3_0": "Paduṭṭho brahmadattena,",
  "ja542:58.4_0": "pāto taṁ ghātayissati”.",
  "ja542:59.1_0": "“Ubbedhati me hadayaṁ,",
  "ja542:59.2_0": "mukhañca parisussati;",
  "ja542:59.3_0": "Nibbutiṁ nādhigacchāmi,",
  "ja542:59.4_0": "aggidaḍḍhova ātape.",
  "ja542:60.1_0": "Kammārānaṁ yathā ukkā,",
  "ja542:60.2_0": "anto jhāyati no bahi;",
  "ja542:60.3_0": "Evampi hadayaṁ mayhaṁ,",
  "ja542:60.4_0": "anto jhāyati no bahi”.",
  "ja542:61.1_0": "“Pamatto mantanātīto,",
  "ja542:61.2_0": "bhinnamantosi khattiya;",
  "ja542:61.3_0": "Idāni kho taṁ tāyantu,",
  "ja542:61.4_0": "paṇḍitā mantino janā.",
  "ja542:62.1_0": "Akatvāmaccassa vacanaṁ,",
  "ja542:62.2_0": "atthakāmahitesino;",
  "ja542:62.3_0": "Attapītirato rājā,",
  "ja542:62.4_0": "migo kūṭeva ohito.",
  "ja542:63.1_0": "Yathāpi maccho baḷisaṁ,",
  "ja542:63.2_0": "vaṅkaṁ maṁsena chāditaṁ;",
  "ja542:63.3_0": "Āmagiddho na jānāti,",
  "ja542:63.4_0": "maccho maraṇamattano.",
  "ja542:64.1_0": "Evameva tuvaṁ rāja,",
  "ja542:64.2_0": "cūḷaneyyassa dhītaraṁ;",
  "ja542:64.3_0": "Kāmagiddho na jānāsi,",
  "ja542:64.4_0": "macchova maraṇamattano.",
  "ja542:65.1_0": "Sace gacchasi pañcālaṁ,",
  "ja542:65.2_0": "khippamattaṁ jahissasi;",
  "ja542:65.3_0": "Migaṁ panthānubandhaṁva,",
  "ja542:65.4_0": "mahantaṁ bhayamessati.",
  "ja542:66.1_0": "Anariyarūpo puriso janinda,",
  "ja542:66.2_0": "Ahīva ucchaṅgagato ḍaseyya;",
  "ja542:66.3_0": "Na tena mittiṁ kayirātha dhīro,",
  "ja542:66.4_0": "Dukkho have kāpurisena saṅgamo.",
  "ja542:67.1_0": "Yadeva jaññā purisaṁ janinda,",
  "ja542:67.2_0": "Sīlavāyaṁ bahussuto;",
  "ja542:67.3_0": "Teneva mittiṁ kayirātha dhīro,",
  "ja542:67.4_0": "Sukho have sappurisena saṅgamo.",
  "ja542:68.1_0": "Bālo tuvaṁ eḷamūgosi rāja,",
  "ja542:68.2_0": "Yo uttamatthāni mayī lapittho;",
  "ja542:68.3_0": "Kimevahaṁ naṅgalakoṭivaḍḍho,",
  "ja542:68.4_0": "Atthāni jānāmi yathāpi aññe.",
  "ja542:69.1_0": "Imaṁ gale gahetvāna,",
  "ja542:69.2_0": "nāsetha vijitā mama;",
  "ja542:69.3_0": "Yo me ratanalābhassa,",
  "ja542:69.4_0": "antarāyāya bhāsati”.",
  "ja542:70.1_0": "“Mahosadha atītena,",
  "ja542:70.2_0": "Nānuvijjhanti paṇḍitā;",
  "ja542:70.3_0": "Kiṁ maṁ assaṁva sambaddhaṁ,",
  "ja542:70.4_0": "Patodeneva vijjhasi.",
  "ja542:71.1_0": "Sace passasi mokkhaṁ vā,",
  "ja542:71.2_0": "khemaṁ vā pana passasi;",
  "ja542:71.3_0": "Teneva maṁ anusāsa,",
  "ja542:71.4_0": "kiṁ atītena vijjhasi”.",
  "ja542:72.1_0": "“Atītaṁ mānusaṁ kammaṁ,",
  "ja542:72.2_0": "Dukkaraṁ durabhisambhavaṁ;",
  "ja542:72.3_0": "Na taṁ sakkomi mocetuṁ,",
  "ja542:72.4_0": "Tvaṁ pajānassu khattiya.",
  "ja542:73.1_0": "Santi vehāyasā nāgā,",
  "ja542:73.2_0": "iddhimanto yasassino;",
  "ja542:73.3_0": "Tepi ādāya gaccheyyuṁ,",
  "ja542:73.4_0": "yassa honti tathāvidhā.",
  "ja542:74.1_0": "Santi vehāyasā assā,",
  "ja542:74.2_0": "iddhimanto yasassino;",
  "ja542:74.3_0": "Tepi ādāya gaccheyyuṁ,",
  "ja542:74.4_0": "yassa honti tathāvidhā.",
  "ja542:75.1_0": "Santi vehāyasā pakkhī,",
  "ja542:75.2_0": "iddhimanto yasassino;",
  "ja542:75.3_0": "Tepi ādāya gaccheyyuṁ,",
  "ja542:75.4_0": "yassa honti tathāvidhā.",
  "ja542:76.1_0": "Santi vehāyasā yakkhā,",
  "ja542:76.2_0": "iddhimanto yasassino;",
  "ja542:76.3_0": "Tepi ādāya gaccheyyuṁ,",
  "ja542:76.4_0": "yassa honti tathāvidhā.",
  "ja542:77.1_0": "Atītaṁ mānusaṁ kammaṁ,",
  "ja542:77.2_0": "Dukkaraṁ durabhisambhavaṁ;",
  "ja542:77.3_0": "Na taṁ sakkomi mocetuṁ,",
  "ja542:77.4_0": "Antalikkhena khattiya”.",
  "ja542:78.1_0": "“Atīradassī puriso,",
  "ja542:78.2_0": "mahante udakaṇṇave;",
  "ja542:78.3_0": "Yattha so labhate gādhaṁ,",
  "ja542:78.4_0": "tattha so vindate sukhaṁ.",
  "ja542:79.1_0": "Evaṁ amhañca rañño ca,",
  "ja542:79.2_0": "tvaṁ patiṭṭhā mahosadha;",
  "ja542:79.3_0": "Tvaṁ nosi mantinaṁ seṭṭho,",
  "ja542:79.4_0": "amhe dukkhā pamocaya”.",
  "ja542:80.1_0": "“Atītaṁ mānusaṁ kammaṁ,",
  "ja542:80.2_0": "Dukkaraṁ durabhisambhavaṁ;",
  "ja542:80.3_0": "Na taṁ sakkomi mocetuṁ,",
  "ja542:80.4_0": "Tvaṁ pajānassu senaka”.",
  "ja542:81.1_0": "“Suṇohi metaṁ vacanaṁ,",
  "ja542:81.2_0": "Passa senaṁ mahabbhayaṁ;",
  "ja542:81.3_0": "Senakaṁ dāni pucchāmi,",
  "ja542:81.4_0": "Kiṁ kiccaṁ idha maññasi”.",
  "ja542:82.1_0": "“Aggiṁ vā dvārato dema,",
  "ja542:82.2_0": "gaṇhāmase vikantanaṁ;",
  "ja542:82.3_0": "Aññamaññaṁ vadhitvāna,",
  "ja542:82.4_0": "khippaṁ hissāma jīvitaṁ;",
  "ja542:82.5_0": "Mā no rājā brahmadatto,",
  "ja542:82.6_0": "ciraṁ dukkhena mārayi”.",
  "ja542:83.1_0": "“Suṇohi metaṁ vacanaṁ,",
  "ja542:83.2_0": "passa senaṁ mahabbhayaṁ;",
  "ja542:83.3_0": "Pukkusaṁ dāni pucchāmi,",
  "ja542:83.4_0": "kiṁ kiccaṁ idha maññasi”.",
  "ja542:84.1_0": "“Visaṁ khāditvā miyyāma,",
  "ja542:84.2_0": "khippaṁ hissāma jīvitaṁ;",
  "ja542:84.3_0": "Mā no rājā brahmadatto,",
  "ja542:84.4_0": "ciraṁ dukkhena mārayi”.",
  "ja542:85.1_0": "“Suṇohi metaṁ vacanaṁ,",
  "ja542:85.2_0": "passa senaṁ mahabbhayaṁ;",
  "ja542:85.3_0": "Kāmindaṁ dāni pucchāmi,",
  "ja542:85.4_0": "kiṁ kiccaṁ idha maññasi”.",
  "ja542:86.1_0": "“Rajjuyā bajjha miyyāma,",
  "ja542:86.2_0": "papātā papatāmase;",
  "ja542:86.3_0": "Mā no rājā brahmadatto,",
  "ja542:86.4_0": "ciraṁ dukkhena mārayi”.",
  "ja542:87.1_0": "“Suṇohi metaṁ vacanaṁ,",
  "ja542:87.2_0": "passa senaṁ mahabbhayaṁ;",
  "ja542:87.3_0": "Devindaṁ dāni pucchāmi,",
  "ja542:87.4_0": "kiṁ kiccaṁ idha maññasi”.",
  "ja542:88.1_0": "“Aggiṁ vā dvārato dema,",
  "ja542:88.2_0": "gaṇhāmase vikantanaṁ;",
  "ja542:88.3_0": "Aññamaññaṁ vadhitvāna,",
  "ja542:88.4_0": "khippaṁ hissāma jīvitaṁ;",
  "ja542:88.5_0": "Na no sakkoti mocetuṁ,",
  "ja542:88.6_0": "sukheneva mahosadho”.",
  "ja542:89.1_0": "“Yathā kadalino sāraṁ,",
  "ja542:89.2_0": "anvesaṁ nādhigacchati;",
  "ja542:89.3_0": "Evaṁ anvesamānā naṁ,",
  "ja542:89.4_0": "pañhaṁ najjhagamāmase.",
  "ja542:90.1_0": "Yathā simbalino sāraṁ,",
  "ja542:90.2_0": "anvesaṁ nādhigacchati;",
  "ja542:90.3_0": "Evaṁ anvesamānā naṁ,",
  "ja542:90.4_0": "pañhaṁ najjhagamāmase.",
  "ja542:91.1_0": "Adese vata no vuṭṭhaṁ,",
  "ja542:91.2_0": "kuñjarānaṁvanodake;",
  "ja542:91.3_0": "Sakāse dummanussānaṁ,",
  "ja542:91.4_0": "bālānaṁ avijānataṁ.",
  "ja542:92.1_0": "Ubbedhati me hadayaṁ,",
  "ja542:92.2_0": "mukhañca parisussati;",
  "ja542:92.3_0": "Nibbutiṁ nādhigacchāmi,",
  "ja542:92.4_0": "aggidaḍḍhova ātape.",
  "ja542:93.1_0": "Kammārānaṁ yathā ukkā,",
  "ja542:93.2_0": "anto jhāyati no bahi;",
  "ja542:93.3_0": "Evampi hadayaṁ mayhaṁ,",
  "ja542:93.4_0": "anto jhāyati no bahi”.",
  "ja542:94.1_0": "Tato so paṇḍito dhīro,",
  "ja542:94.2_0": "atthadassī mahosadho;",
  "ja542:94.3_0": "Vedehaṁ dukkhitaṁ disvā,",
  "ja542:94.4_0": "idaṁ vacanamabravi.",
  "ja542:95.1_0": "“Mā tvaṁ bhāyi mahārāja,",
  "ja542:95.2_0": "mā tvaṁ bhāyi rathesabha;",
  "ja542:95.3_0": "Ahaṁ taṁ mocayissāmi,",
  "ja542:95.4_0": "rāhuggahaṁva candimaṁ.",
  "ja542:96.1_0": "Mā tvaṁ bhāyi mahārāja,",
  "ja542:96.2_0": "mā tvaṁ bhāyi rathesabha;",
  "ja542:96.3_0": "Ahaṁ taṁ mocayissāmi,",
  "ja542:96.4_0": "rāhuggahaṁva sūriyaṁ.",
  "ja542:97.1_0": "Mā tvaṁ bhāyi mahārāja,",
  "ja542:97.2_0": "mā tvaṁ bhāyi rathesabha;",
  "ja542:97.3_0": "Ahaṁ taṁ mocayissāmi,",
  "ja542:97.4_0": "paṅke sannaṁva kuñjaraṁ.",
  "ja542:98.1_0": "Mā tvaṁ bhāyi mahārāja,",
  "ja542:98.2_0": "mā tvaṁ bhāyi rathesabha;",
  "ja542:98.3_0": "Ahaṁ taṁ mocayissāmi,",
  "ja542:98.4_0": "peḷābaddhaṁva pannagaṁ.",
  "ja542:99.1_0": "Mā tvaṁ bhāyi mahārāja,",
  "ja542:99.2_0": "mā tvaṁ bhāyi rathesabha;",
  "ja542:99.3_0": "Ahaṁ taṁ mocayissāmi,",
  "ja542:99.4_0": "pakkhiṁ baddhaṁva pañjare.",
  "ja542:100.1_0": "Mā tvaṁ bhāyi mahārāja,",
  "ja542:100.2_0": "mā tvaṁ bhāyi rathesabha;",
  "ja542:100.3_0": "Ahaṁ taṁ mocayissāmi,",
  "ja542:100.4_0": "macche jālagateriva.",
  "ja542:101.1_0": "Mā tvaṁ bhāyi mahārāja,",
  "ja542:101.2_0": "mā tvaṁ bhāyi rathesabha;",
  "ja542:101.3_0": "Ahaṁ taṁ mocayissāmi,",
  "ja542:101.4_0": "sayoggabalavāhanaṁ.",
  "ja542:102.1_0": "Mā tvaṁ bhāyi mahārāja,",
  "ja542:102.2_0": "mā tvaṁ bhāyi rathesabha;",
  "ja542:102.3_0": "Pañcālaṁ vāhayissāmi,",
  "ja542:102.4_0": "kākasenaṁva leḍḍunā.",
  "ja542:103.1_0": "Adu paññā kimatthiyā,",
  "ja542:103.2_0": "Amacco vāpi tādiso;",
  "ja542:103.3_0": "Yo taṁ sambādhapakkhandaṁ,",
  "ja542:103.4_0": "Dukkhā na parimocaye”.",
  "ja542:104.1_0": "“Etha māṇavā uṭṭhetha,",
  "ja542:104.2_0": "mukhaṁ sodhetha sandhino;",
  "ja542:104.3_0": "Vedeho sahamaccehi,",
  "ja542:104.4_0": "umaṅgena gamissati”.",
  "ja542:105.1_0": "Tassa taṁ vacanaṁ sutvā,",
  "ja542:105.2_0": "paṇḍitassānucārino;",
  "ja542:105.3_0": "Umaṅgadvāraṁ vivariṁsu,",
  "ja542:105.4_0": "yantayutte ca aggaḷe.",
  "ja542:106.1_0": "Purato senako yāti,",
  "ja542:106.2_0": "pacchato ca mahosadho;",
  "ja542:106.3_0": "Majjhe ca rājā vedeho,",
  "ja542:106.4_0": "amaccaparivārito.",
  "ja542:107.1_0": "Umaṅgā nikkhamitvāna,",
  "ja542:107.2_0": "vedeho nāvamāruhi;",
  "ja542:107.3_0": "Abhirūḷhañca taṁ ñatvā,",
  "ja542:107.4_0": "anusāsi mahosadho.",
  "ja542:108.1_0": "“Ayaṁ te sasuro deva,",
  "ja542:108.2_0": "ayaṁ sassu janādhipa;",
  "ja542:108.3_0": "Yathā mātu paṭipatti,",
  "ja542:108.4_0": "evaṁ te hotu sassuyā.",
  "ja542:109.1_0": "Yathāpi niyako bhātā,",
  "ja542:109.2_0": "Saudariyo ekamātuko;",
  "ja542:109.3_0": "Evaṁ pañcālacando te,",
  "ja542:109.4_0": "Dayitabbo rathesabha.",
  "ja542:110.1_0": "Ayaṁ pañcālacandī te,",
  "ja542:110.2_0": "rājaputtī abhicchitā;",
  "ja542:110.3_0": "Kāmaṁ karohi te tāya,",
  "ja542:110.4_0": "bhariyā te rathesabha”.",
  "ja542:111.1_0": "“Āruyha nāvaṁ taramāno,",
  "ja542:111.2_0": "kiṁ nu tīramhi tiṭṭhasi;",
  "ja542:111.3_0": "Kicchā muttāmha dukkhato,",
  "ja542:111.4_0": "yāma dāni mahosadha”.",
  "ja542:112.1_0": "“Nesa dhammo mahārāja,",
  "ja542:112.2_0": "yohaṁ senāya nāyako;",
  "ja542:112.3_0": "Senaṅgaṁ parihāpetvā,",
  "ja542:112.4_0": "attānaṁ parimocaye.",
  "ja542:113.1_0": "Nivesanamhi te deva,",
  "ja542:113.2_0": "senaṅgaṁ parihāpitaṁ;",
  "ja542:113.3_0": "Taṁ dinnaṁ brahmadattena,",
  "ja542:113.4_0": "ānayissaṁ rathesabha”.",
  "ja542:114.1_0": "“Appaseno mahāsenaṁ,",
  "ja542:114.2_0": "kathaṁ viggayha ṭhassasi;",
  "ja542:114.3_0": "Dubbalo balavantena,",
  "ja542:114.4_0": "vihaññissasi paṇḍita”.",
  "ja542:115.1_0": "“Appasenopi ce mantī,",
  "ja542:115.2_0": "mahāsenaṁ amantinaṁ;",
  "ja542:115.3_0": "Jināti rājā rājāno,",
  "ja542:115.4_0": "ādiccovudayaṁ tamaṁ”.",
  "ja542:116.1_0": "“Susukhaṁ vata saṁvāso,",
  "ja542:116.2_0": "paṇḍitehīti senaka;",
  "ja542:116.3_0": "Pakkhīva pañjare baddhe,",
  "ja542:116.4_0": "macche jālagateriva;",
  "ja542:116.5_0": "Amittahatthattagate,",
  "ja542:116.6_0": "mocayī no mahosadho”.",
  "ja542:117.1_0": "“Evametaṁ mahārāja,",
  "ja542:117.2_0": "paṇḍitā hi sukhāvahā;",
  "ja542:117.3_0": "Pakkhīva pañjare baddhe,",
  "ja542:117.4_0": "macche jālagateriva;",
  "ja542:117.5_0": "Amittahatthattagate,",
  "ja542:117.6_0": "mocayī no mahosadho”.",
  "ja542:118.1_0": "Rakkhitvā kasiṇaṁ rattiṁ,",
  "ja542:118.2_0": "cūḷaneyyo mahabbalo;",
  "ja542:118.3_0": "Udentaṁ aruṇuggasmiṁ,",
  "ja542:118.4_0": "upakāriṁ upāgami.",
  "ja542:119.1_0": "Āruyha pavaraṁ nāgaṁ,",
  "ja542:119.2_0": "balavantaṁ saṭṭhihāyanaṁ;",
  "ja542:119.3_0": "Rājā avoca pañcālo,",
  "ja542:119.4_0": "cūḷaneyyo mahabbalo.",
  "ja542:120.1_0": "Sannaddho maṇivammena,",
  "ja542:120.2_0": "saramādāya pāṇinā;",
  "ja542:120.3_0": "Pesiye ajjhabhāsittha,",
  "ja542:120.4_0": "puthugumbe samāgate.",
  "ja542:121.1_0": "Hatthārohe anīkaṭṭhe,",
  "ja542:121.2_0": "rathike pattikārake;",
  "ja542:121.3_0": "Upāsanamhi katahatthe,",
  "ja542:121.4_0": "vālavedhe samāgate.",
  "ja542:122.1_0": "“Pesetha kuñjare dantī,",
  "ja542:122.2_0": "balavante saṭṭhihāyane;",
  "ja542:122.3_0": "Maddantu kuñjarā nagaraṁ,",
  "ja542:122.4_0": "vedehena sumāpitaṁ.",
  "ja542:123.1_0": "Vacchadantamukhā setā,",
  "ja542:123.2_0": "tikkhaggā aṭṭhivedhino;",
  "ja542:123.3_0": "Paṇunnā dhanuvegena,",
  "ja542:123.4_0": "sampatantutarītarā.",
  "ja542:124.1_0": "Māṇavā vammino sūrā,",
  "ja542:124.2_0": "citradaṇḍayutāvudhā;",
  "ja542:124.3_0": "Pakkhandino mahānāgā,",
  "ja542:124.4_0": "hatthīnaṁ hontu sammukhā.",
  "ja542:125.1_0": "Sattiyo teladhotāyo,",
  "ja542:125.2_0": "accimantā pabhassarā;",
  "ja542:125.3_0": "Vijjotamānā tiṭṭhantu,",
  "ja542:125.4_0": "sataraṁsīva tārakā.",
  "ja542:126.1_0": "Āvudhabalavantānaṁ,",
  "ja542:126.2_0": "guṇikāyūradhārinaṁ;",
  "ja542:126.3_0": "Etādisānaṁ yodhānaṁ,",
  "ja542:126.4_0": "saṅgāme apalāyinaṁ;",
  "ja542:126.5_0": "Vedeho kuto muccissati,",
  "ja542:126.6_0": "sace pakkhīva kāhiti.",
  "ja542:127.1_0": "Tiṁsa me purisanāvutyo,",
  "ja542:127.2_0": "sabbevekekaniccitā;",
  "ja542:127.3_0": "Yesaṁ samaṁ na passāmi,",
  "ja542:127.4_0": "kevalaṁ mahimaṁ caraṁ.",
  "ja542:128.1_0": "Nāgā ca kappitā dantī,",
  "ja542:128.2_0": "balavanto saṭṭhihāyanā;",
  "ja542:128.3_0": "Yesaṁ khandhesu sobhanti,",
  "ja542:128.4_0": "kumārā cārudassanā.",
  "ja542:129.1_0": "Pītālaṅkārā pītavasanā,",
  "ja542:129.2_0": "Pītuttaranivāsanā;",
  "ja542:129.3_0": "Nāgakhandhesu sobhanti,",
  "ja542:129.4_0": "Devaputtāva nandane.",
  "ja542:130.1_0": "Pāṭhīnavaṇṇā nettiṁsā,",
  "ja542:130.2_0": "teladhotā pabhassarā;",
  "ja542:130.3_0": "Niṭṭhitā naradhīrehi,",
  "ja542:130.4_0": "samadhārā sunissitā.",
  "ja542:131.1_0": "Vellālino vītamalā,",
  "ja542:131.2_0": "sikkāyasamayā daḷā;",
  "ja542:131.3_0": "Gahitā balavantehi,",
  "ja542:131.4_0": "suppahārappahāribhi.",
  "ja542:132.1_0": "Suvaṇṇatharusampannā,",
  "ja542:132.2_0": "lohitakacchupadhāritā;",
  "ja542:132.3_0": "Vivattamānā sobhanti,",
  "ja542:132.4_0": "vijjuvabbhaghanantare.",
  "ja542:133.1_0": "Paṭākā vammino sūrā,",
  "ja542:133.2_0": "asicammassa kovidā;",
  "ja542:133.3_0": "Dhanuggahā sikkhitarā,",
  "ja542:133.4_0": "nāgakhandhe nipātino.",
  "ja542:134.1_0": "Etādisehi parikkhitto,",
  "ja542:134.2_0": "natthi mokkho ito tava;",
  "ja542:134.3_0": "Pabhāvaṁ te na passāmi,",
  "ja542:134.4_0": "yena tvaṁ mithilaṁ vaje”.",
  "ja542:135.1_0": "“Kiṁ nu santaramānova,",
  "ja542:135.2_0": "nāgaṁ pesesi kuñjaraṁ;",
  "ja542:135.3_0": "Pahaṭṭharūpo āpatasi,",
  "ja542:135.4_0": "siddhatthosmīti maññasi.",
  "ja542:136.1_0": "Oharetaṁ dhanuṁ cāpaṁ,",
  "ja542:136.2_0": "khurappaṁ paṭisaṁhara;",
  "ja542:136.3_0": "Oharetaṁ subhaṁ vammaṁ,",
  "ja542:136.4_0": "veḷuriyamaṇisanthataṁ”.",
  "ja542:137.1_0": "“Pasannamukhavaṇṇosi,",
  "ja542:137.2_0": "mitapubbañca bhāsasi;",
  "ja542:137.3_0": "Hoti kho maraṇakāle,",
  "ja542:137.4_0": "edisī vaṇṇasampadā”.",
  "ja542:138.1_0": "“Moghaṁ te gajjitaṁ rāja,",
  "ja542:138.2_0": "bhinnamantosi khattiya;",
  "ja542:138.3_0": "Duggaṇhosi tayā rājā,",
  "ja542:138.4_0": "khaḷuṅkeneva sindhavo.",
  "ja542:139.1_0": "Tiṇṇo hiyyo rājā gaṅgaṁ,",
  "ja542:139.2_0": "sāmacco saparijjano;",
  "ja542:139.3_0": "Haṁsarājaṁ yathā dhaṅko,",
  "ja542:139.4_0": "anujjavaṁ patissasi.",
  "ja542:140.1_0": "Siṅgālā rattibhāgena,",
  "ja542:140.2_0": "phullaṁ disvāna kiṁsukaṁ;",
  "ja542:140.3_0": "Maṁsapesīti maññantā,",
  "ja542:140.4_0": "paribyūḷhā migādhamā.",
  "ja542:141.1_0": "Vītivattāsu rattīsu,",
  "ja542:141.2_0": "uggatasmiṁ divākare;",
  "ja542:141.3_0": "Kiṁsukaṁ phullitaṁ disvā,",
  "ja542:141.4_0": "āsacchinnā migādhamā.",
  "ja542:142.1_0": "Evameva tuvaṁ rāja,",
  "ja542:142.2_0": "vedehaṁ parivāriya;",
  "ja542:142.3_0": "Āsacchinno gamissasi,",
  "ja542:142.4_0": "siṅgālā kiṁsukaṁ yathā”.",
  "ja542:143.1_0": "“Imassa hatthe pāde ca,",
  "ja542:143.2_0": "kaṇṇanāsañca chindatha;",
  "ja542:143.3_0": "Yo me amittaṁ hatthagataṁ,",
  "ja542:143.4_0": "vedehaṁ parimocayi.",
  "ja542:144.1_0": "Imaṁ maṁsaṁva pātabyaṁ,",
  "ja542:144.2_0": "sūle katvā pacantu naṁ;",
  "ja542:144.3_0": "Yo me amittaṁ hatthagataṁ,",
  "ja542:144.4_0": "vedehaṁ parimocayi.",
  "ja542:145.1_0": "Yathāpi āsabhaṁ cammaṁ,",
  "ja542:145.2_0": "pathabyā vitaniyyati;",
  "ja542:145.3_0": "Sīhassa atho byagghassa,",
  "ja542:145.4_0": "hoti saṅkusamāhataṁ.",
  "ja542:146.1_0": "Evaṁ taṁ vitanitvāna,",
  "ja542:146.2_0": "vedhayissāmi sattiyā;",
  "ja542:146.3_0": "Yo me amittaṁ hatthagataṁ,",
  "ja542:146.4_0": "vedehaṁ parimocayi”.",
  "ja542:147.1_0": "“Sace me hatthe pāde ca,",
  "ja542:147.2_0": "kaṇṇanāsañca checchasi;",
  "ja542:147.3_0": "Evaṁ pañcālacandassa,",
  "ja542:147.4_0": "vedeho chedayissati.",
  "ja542:148.1_0": "Sace me hatthe pāde ca,",
  "ja542:148.2_0": "kaṇṇanāsañca checchasi;",
  "ja542:148.3_0": "Evaṁ pañcālacandiyā,",
  "ja542:148.4_0": "vedeho chedayissati.",
  "ja542:149.1_0": "Sace me hatthe pāde ca,",
  "ja542:149.2_0": "kaṇṇanāsañca checchasi;",
  "ja542:149.3_0": "Evaṁ nandāya deviyā,",
  "ja542:149.4_0": "vedeho chedayissati.",
  "ja542:150.1_0": "Sace me hatthe pāde ca,",
  "ja542:150.2_0": "kaṇṇanāsañca checchasi;",
  "ja542:150.3_0": "Evaṁ te puttadārassa,",
  "ja542:150.4_0": "vedeho chedayissati.",
  "ja542:151.1_0": "Sace maṁsaṁva pātabyaṁ,",
  "ja542:151.2_0": "sūle katvā pacissasi;",
  "ja542:151.3_0": "Evaṁ pañcālacandassa,",
  "ja542:151.4_0": "vedeho pācayissati.",
  "ja542:152.1_0": "Sace maṁsaṁva pātabyaṁ,",
  "ja542:152.2_0": "sūle katvā pacissasi;",
  "ja542:152.3_0": "Evaṁ pañcālacandiyā,",
  "ja542:152.4_0": "vedeho pācayissati.",
  "ja542:153.1_0": "Sace maṁsaṁva pātabyaṁ,",
  "ja542:153.2_0": "sūle katvā pacissasi;",
  "ja542:153.3_0": "Evaṁ nandāya deviyā,",
  "ja542:153.4_0": "vedeho pācayissati.",
  "ja542:154.1_0": "Sace maṁsaṁva pātabyaṁ,",
  "ja542:154.2_0": "sūle katvā pacissasi;",
  "ja542:154.3_0": "Evaṁ te puttadārassa,",
  "ja542:154.4_0": "vedeho pācayissati.",
  "ja542:155.1_0": "Sace maṁ vitanitvāna,",
  "ja542:155.2_0": "vedhayissasi sattiyā;",
  "ja542:155.3_0": "Evaṁ pañcālacandassa,",
  "ja542:155.4_0": "vedeho vedhayissati.",
  "ja542:156.1_0": "Sace maṁ vitanitvāna,",
  "ja542:156.2_0": "vedhayissasi sattiyā;",
  "ja542:156.3_0": "Evaṁ pañcālacandiyā,",
  "ja542:156.4_0": "vedeho vedhayissati.",
  "ja542:157.1_0": "Sace maṁ vitanitvāna,",
  "ja542:157.2_0": "vedhayissasi sattiyā;",
  "ja542:157.3_0": "Evaṁ nandāya deviyā,",
  "ja542:157.4_0": "vedeho vedhayissati.",
  "ja542:158.1_0": "Sace maṁ vitanitvāna,",
  "ja542:158.2_0": "vedhayissasi sattiyā;",
  "ja542:158.3_0": "Evaṁ te puttadārassa,",
  "ja542:158.4_0": "vedeho vedhayissati;",
  "ja542:158.5_0": "Evaṁ no mantitaṁ raho,",
  "ja542:158.6_0": "vedehena mayā saha.",
  "ja542:159.1_0": "Yathāpi palasataṁ cammaṁ,",
  "ja542:159.2_0": "kontimantāsuniṭṭhitaṁ;",
  "ja542:159.3_0": "Upeti tanutāṇāya,",
  "ja542:159.4_0": "sarānaṁ paṭihantave.",
  "ja542:160.1_0": "Sukhāvaho dukkhanudo,",
  "ja542:160.2_0": "vedehassa yasassino;",
  "ja542:160.3_0": "Matiṁ te paṭihaññāmi,",
  "ja542:160.4_0": "usuṁ palasatena vā.",
  "ja542:161.1_0": "Iṅgha passa mahārāja,",
  "ja542:161.2_0": "suññaṁ antepuraṁ tava;",
  "ja542:161.3_0": "Orodhā ca kumārā ca,",
  "ja542:161.4_0": "tava mātā ca khattiya;",
  "ja542:161.5_0": "Umaṅgā nīharitvāna,",
  "ja542:161.6_0": "vedehassupanāmitā”.",
  "ja542:162.1_0": "“Iṅgha antepuraṁ mayhaṁ,",
  "ja542:162.2_0": "gantvāna vicinātha naṁ;",
  "ja542:162.3_0": "Yathā imassa vacanaṁ,",
  "ja542:162.4_0": "saccaṁ vā yadi vā musā”.",
  "ja542:163.1_0": "“Evametaṁ mahārāja,",
  "ja542:163.2_0": "yathā āha mahosadho;",
  "ja542:163.3_0": "Suññaṁ antepuraṁ sabbaṁ,",
  "ja542:163.4_0": "kākapaṭṭanakaṁ yathā”.",
  "ja542:164.1_0": "“Ito gatā mahārāja,",
  "ja542:164.2_0": "nārī sabbaṅgasobhanā;",
  "ja542:164.3_0": "Kosambaphalakasussoṇī,",
  "ja542:164.4_0": "haṁsagaggarabhāṇinī.",
  "ja542:165.1_0": "Ito nītā mahārāja,",
  "ja542:165.2_0": "nārī sabbaṅgasobhanā;",
  "ja542:165.3_0": "Koseyyavasanā sāmā,",
  "ja542:165.4_0": "jātarūpasumekhalā.",
  "ja542:166.1_0": "Surattapādā kalyāṇī,",
  "ja542:166.2_0": "suvaṇṇamaṇimekhalā;",
  "ja542:166.3_0": "Pārevatakkhī sutanū,",
  "ja542:166.4_0": "bimboṭṭhā tanumajjhimā.",
  "ja542:167.1_0": "Sujātā bhujalaṭṭhīva,",
  "ja542:167.2_0": "vedīva tanumajjhimā;",
  "ja542:167.3_0": "Dīghassā kesā asitā,",
  "ja542:167.4_0": "īsakaggapavellitā.",
  "ja542:168.1_0": "Sujātā migachāpāva,",
  "ja542:168.2_0": "hemantaggisikhāriva;",
  "ja542:168.3_0": "Nadīva giriduggesu,",
  "ja542:168.4_0": "sañchannā khuddaveḷubhi.",
  "ja542:169.1_0": "Nāganāsūru kalyāṇī,",
  "ja542:169.2_0": "paramā timbarutthanī;",
  "ja542:169.3_0": "Nātidīghā nātirassā,",
  "ja542:169.4_0": "nālomā nātilomasā”.",
  "ja542:170.1_0": "“Nandāya nūna maraṇena,",
  "ja542:170.2_0": "nandasi sirivāhana;",
  "ja542:170.3_0": "Ahañca nūna nandā ca,",
  "ja542:170.4_0": "gacchāma yamasādhanaṁ”.",
  "ja542:171.1_0": "“Dibbaṁ adhīyase māyaṁ,",
  "ja542:171.2_0": "akāsi cakkhumohanaṁ;",
  "ja542:171.3_0": "Yo me amittaṁ hatthagataṁ,",
  "ja542:171.4_0": "vedehaṁ parimocayi”.",
  "ja542:172.1_0": "“Adhīyanti mahārāja,",
  "ja542:172.2_0": "dibbamāyidha paṇḍitā;",
  "ja542:172.3_0": "Te mocayanti attānaṁ,",
  "ja542:172.4_0": "paṇḍitā mantino janā.",
  "ja542:173.1_0": "Santi māṇavaputtā me,",
  "ja542:173.2_0": "kusalā sandhichedakā;",
  "ja542:173.3_0": "Yesaṁ katena maggena,",
  "ja542:173.4_0": "vedeho mithilaṁ gato”.",
  "ja542:174.1_0": "“Iṅgha passa mahārāja,",
  "ja542:174.2_0": "Umaṅgaṁ sādhu māpitaṁ;",
  "ja542:174.3_0": "Hatthīnaṁ atha assānaṁ,",
  "ja542:174.4_0": "Rathānaṁ atha pattinaṁ;",
  "ja542:174.5_0": "Ālokabhūtaṁ tiṭṭhantaṁ,",
  "ja542:174.6_0": "Umaṅgaṁ sādhu māpitaṁ”.",
  "ja542:175.1_0": "“Lābhā vata videhānaṁ,",
  "ja542:175.2_0": "yassimedisā paṇḍitā;",
  "ja542:175.3_0": "Ghare vasanti vijite,",
  "ja542:175.4_0": "yathā tvaṁsi mahosadha”.",
  "ja542:176.1_0": "“Vuttiñca parihārañca,",
  "ja542:176.2_0": "diguṇaṁ bhattavetanaṁ;",
  "ja542:176.3_0": "Dadāmi vipule bhoge,",
  "ja542:176.4_0": "bhuñja kāme ramassu ca;",
  "ja542:176.5_0": "Mā videhaṁ paccagamā,",
  "ja542:176.6_0": "kiṁ videho karissati”.",
  "ja542:177.1_0": "“Yo cajetha mahārāja,",
  "ja542:177.2_0": "bhattāraṁ dhanakāraṇā;",
  "ja542:177.3_0": "Ubhinnaṁ hoti gārayho,",
  "ja542:177.4_0": "attano ca parassa ca;",
  "ja542:177.5_0": "Yāva jīveyya vedeho,",
  "ja542:177.6_0": "nāññassa puriso siyā.",
  "ja542:178.1_0": "Yo cajetha mahārāja,",
  "ja542:178.2_0": "bhattāraṁ dhanakāraṇā;",
  "ja542:178.3_0": "Ubhinnaṁ hoti gārayho,",
  "ja542:178.4_0": "attano ca parassa ca;",
  "ja542:178.5_0": "Yāva tiṭṭheyya vedeho,",
  "ja542:178.6_0": "nāññassa vijite vase”.",
  "ja542:179.1_0": "“Dammi nikkhasahassaṁ te,",
  "ja542:179.2_0": "gāmāsītiñca kāsisu;",
  "ja542:179.3_0": "Dāsisatāni cattāri,",
  "ja542:179.4_0": "dammi bhariyāsatañca te;",
  "ja542:179.5_0": "Sabbaṁ senaṅgamādāya,",
  "ja542:179.6_0": "sotthiṁ gaccha mahosadha.",
  "ja542:180.1_0": "Yāva dadantu hatthīnaṁ,",
  "ja542:180.2_0": "assānaṁ diguṇaṁ vidhaṁ;",
  "ja542:180.3_0": "Tappentu annapānena,",
  "ja542:180.4_0": "rathike pattikārake.",
  "ja542:181.1_0": "Hatthī asse rathe pattī,",
  "ja542:181.2_0": "gacchevādāya paṇḍita;",
  "ja542:181.3_0": "Passatu taṁ mahārājā,",
  "ja542:181.4_0": "vedeho mithilaṁ gataṁ”.",
  "ja542:182.1_0": "“Hatthī assā rathā pattī,",
  "ja542:182.2_0": "senā padissate mahā;",
  "ja542:182.3_0": "Caturaṅginī bhīsarūpā,",
  "ja542:182.4_0": "kiṁ nu maññasi paṇḍita”.",
  "ja542:183.1_0": "“Ānando te mahārāja,",
  "ja542:183.2_0": "uttamo paṭidissati;",
  "ja542:183.3_0": "Sabbaṁ senaṅgamādāya,",
  "ja542:183.4_0": "sotthiṁ patto mahosadho”.",
  "ja542:184.1_0": "“Yathā petaṁ susānasmiṁ,",
  "ja542:184.2_0": "chaḍḍetvā caturo janā;",
  "ja542:184.3_0": "Evaṁ kapilaye tyamha,",
  "ja542:184.4_0": "chaḍḍayitvā idhāgatā.",
  "ja542:185.1_0": "Atha tvaṁ kena vaṇṇena,",
  "ja542:185.2_0": "kena vā pana hetunā;",
  "ja542:185.3_0": "Kena vā atthajātena,",
  "ja542:185.4_0": "attānaṁ parimocayi”.",
  "ja542:186.1_0": "“Atthaṁ atthena vedeha,",
  "ja542:186.2_0": "mantaṁ mantena khattiya;",
  "ja542:186.3_0": "Parivārayiṁ rājānaṁ,",
  "ja542:186.4_0": "jambudīpaṁva sāgaro”.",
  "ja542:187.1_0": "“Dinnaṁ nikkhasahassaṁ me,",
  "ja542:187.2_0": "gāmāsīti ca kāsisu;",
  "ja542:187.3_0": "Dāsīsatāni cattāri,",
  "ja542:187.4_0": "dinnaṁ bhariyāsatañca me;",
  "ja542:187.5_0": "Sabbaṁ senaṅgamādāya,",
  "ja542:187.6_0": "sotthināmhi idhāgato”.",
  "ja542:188.1_0": "“Susukhaṁ vata saṁvāso,",
  "ja542:188.2_0": "paṇḍitehīti senaka;",
  "ja542:188.3_0": "Pakkhīva pañjare baddhe,",
  "ja542:188.4_0": "macche jālagateriva;",
  "ja542:188.5_0": "Amittahatthattagate,",
  "ja542:188.6_0": "mocayī no mahosadho”.",
  "ja542:189.1_0": "“Evametaṁ mahārāja,",
  "ja542:189.2_0": "paṇḍitā hi sukhāvahā;",
  "ja542:189.3_0": "Pakkhīva pañjare baddhe,",
  "ja542:189.4_0": "macche jālagateriva;",
  "ja542:189.5_0": "Amittahatthattagate,",
  "ja542:189.6_0": "mocayī no mahosadho”.",
  "ja542:190.1_0": "“Āhaññantu sabbavīṇā,",
  "ja542:190.2_0": "bheriyo dindimāni ca;",
  "ja542:190.3_0": "Dhamentu māgadhā saṅkhā,",
  "ja542:190.4_0": "vaggū nadantu dundubhī”.",
  "ja542:191.1_0": "Orodhā ca kumārā ca,",
  "ja542:191.2_0": "vesiyānā ca brāhmaṇā;",
  "ja542:191.3_0": "Bahuṁ annañca pānañca,",
  "ja542:191.4_0": "paṇḍitassābhihārayuṁ.",
  "ja542:192.1_0": "Hatthārohā anīkaṭṭhā,",
  "ja542:192.2_0": "rathikā pattikārakā;",
  "ja542:192.3_0": "Bahuṁ annañca pānañca,",
  "ja542:192.4_0": "paṇḍitassābhihārayuṁ.",
  "ja542:193.1_0": "Samāgatā jānapadā,",
  "ja542:193.2_0": "negamā ca samāgatā;",
  "ja542:193.3_0": "Bahuṁ annañca pānañca,",
  "ja542:193.4_0": "paṇḍitassābhihārayuṁ.",
  "ja542:194.1_0": "Bahujano pasannosi,",
  "ja542:194.2_0": "disvā paṇḍitamāgataṁ;",
  "ja542:194.3_0": "Paṇḍitamhi anuppatte,",
  "ja542:194.4_0": "celukkhepo avattathāti.",
  "ja542:195.1_0": "Umaṅgajātakaṁ pañcamaṁ."
}