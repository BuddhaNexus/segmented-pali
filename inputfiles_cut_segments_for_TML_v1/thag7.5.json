{
  "thag7.5:0.1_0": "Theragāthā",
  "thag7.5:0.2_0": "Sattakanipāta",
  "thag7.5:0.3_0": "Paṭhamavagga",
  "thag7.5:0.4_0": "7.5. Sarabhaṅgattheragāthā",
  "thag7.5:1.1_0": "“Sare hatthehi bhañjitvā,",
  "thag7.5:1.2_0": "katvāna kuṭimacchisaṁ;",
  "thag7.5:1.3_0": "Tena me sarabhaṅgoti,",
  "thag7.5:1.4_0": "nāmaṁ sammutiyā ahu.",
  "thag7.5:2.1_0": "Na mayhaṁ kappate ajja,",
  "thag7.5:2.2_0": "sare hatthehi bhañjituṁ;",
  "thag7.5:2.3_0": "Sikkhāpadā no paññattā,",
  "thag7.5:2.4_0": "gotamena yasassinā.",
  "thag7.5:3.1_0": "Sakalaṁ samattaṁ rogaṁ,",
  "thag7.5:3.2_0": "Sarabhaṅgo nāddasaṁ pubbe;",
  "thag7.5:3.3_0": "Soyaṁ rogo diṭṭho,",
  "thag7.5:3.4_0": "Vacanakarenātidevassa.",
  "thag7.5:4.1_0": "Yeneva maggena gato vipassī,",
  "thag7.5:4.2_0": "Yeneva maggena sikhī ca vessabhū;",
  "thag7.5:4.3_0": "Kakusandhakoṇāgamano ca kassapo,",
  "thag7.5:4.4_0": "Tenañjasena agamāsi gotamo.",
  "thag7.5:5.1_0": "Vītataṇhā anādānā,",
  "thag7.5:5.2_0": "satta buddhā khayogadhā;",
  "thag7.5:5.3_0": "Yehāyaṁ desito dhammo,",
  "thag7.5:5.4_0": "dhammabhūtehi tādibhi.",
  "thag7.5:6.1_0": "Cattāri ariyasaccāni,",
  "thag7.5:6.2_0": "anukampāya pāṇinaṁ;",
  "thag7.5:6.3_0": "Dukkhaṁ samudayo maggo,",
  "thag7.5:6.4_0": "nirodho dukkhasaṅkhayo.",
  "thag7.5:7.1_0": "Yasmiṁ nivattate dukkhaṁ,",
  "thag7.5:7.2_0": "saṁsārasmiṁ anantakaṁ;",
  "thag7.5:7.3_0": "Bhedā imassa kāyassa,",
  "thag7.5:7.4_0": "jīvitassa ca saṅkhayā;",
  "thag7.5:7.5_0": "Añño punabbhavo natthi,",
  "thag7.5:7.6_0": "suvimuttomhi sabbadhī”ti.",
  "thag7.5:7.7_0": "…",
  "thag7.5:7.8_0": "Sarabhaṅgo thero ….",
  "thag7.5:7.9_0": "Sattakanipāto niṭṭhito.",
  "thag7.5:8.0_0": "Tatruddānaṁ",
  "thag7.5:8.1_0": "Sundarasamuddo thero,",
  "thag7.5:8.2_0": "thero lakuṇḍabhaddiyo;",
  "thag7.5:8.3_0": "Bhaddo thero ca sopāko,",
  "thag7.5:8.4_0": "sarabhaṅgo mahāisi;",
  "thag7.5:8.5_0": "Sattake pañcakā therā,",
  "thag7.5:8.6_0": "gāthāyo pañcatiṁsatīti."
}