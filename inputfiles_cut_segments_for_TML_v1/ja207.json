{
  "ja207:0.1_0": "Jātaka",
  "ja207:0.2_0": "Dukanipāta",
  "ja207:0.3_0": "Nataṁdaḷhavagga",
  "ja207:0.4_0": "7. Assakajātaka",
  "ja207:1.1_0": "“Ayamassakarājena,",
  "ja207:1.2_0": "deso vicarito mayā;",
  "ja207:1.3_0": "Anukāmayakāmena,",
  "ja207:1.4_0": "piyena patinā saha.",
  "ja207:2.1_0": "Navena sukhadukkhena,",
  "ja207:2.2_0": "porāṇaṁ apidhīyati;",
  "ja207:2.3_0": "Tasmā assakaraññāva,",
  "ja207:2.4_0": "kīṭo piyataro mamā”ti.",
  "ja207:3.1_0": "Assakajātakaṁ sattamaṁ."
}