{
  "tha-ap249:0.1_0": "Therāpadāna",
  "tha-ap249:0.2_0": "Tuvaradāyakavagga",
  "tha-ap249:0.3_0": "7. Dhātupūjakattheraapadāna",
  "tha-ap249:1.1_0": "“Nibbute lokanāthamhi,",
  "tha-ap249:1.2_0": "siddhatthamhi naruttame;",
  "tha-ap249:1.3_0": "Ekā dhātu mayā laddhā,",
  "tha-ap249:1.4_0": "dvipadindassa tādino.",
  "tha-ap249:2.1_0": "Tāhaṁ dhātuṁ gahetvāna,",
  "tha-ap249:2.2_0": "buddhassādiccabandhuno;",
  "tha-ap249:2.3_0": "Pañcavasse paricariṁ,",
  "tha-ap249:2.4_0": "tiṭṭhantaṁva naruttamaṁ.",
  "tha-ap249:3.1_0": "Catunnavutito kappe,",
  "tha-ap249:3.2_0": "yaṁ dhātuṁ pūjayiṁ tadā;",
  "tha-ap249:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap249:3.4_0": "dhātupaṭṭhahane phalaṁ.",
  "tha-ap249:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap249:4.2_0": "…pe…",
  "tha-ap249:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap249:5.1_0": "Itthaṁ sudaṁ āyasmā dhātupūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap249:6.1_0": "Dhātupūjakattherassāpadānaṁ sattamaṁ."
}