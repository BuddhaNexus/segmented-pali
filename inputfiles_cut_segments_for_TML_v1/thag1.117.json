{
  "thag1.117:0.1_0": "Theragāthā",
  "thag1.117:0.2_0": "Ekakanipāta",
  "thag1.117:0.3_0": "Dvādasamavagga",
  "thag1.117:0.4_0": "1.117. Yasattheragāthā",
  "thag1.117:1.1_0": "“Suvilitto suvasano,",
  "thag1.117:1.2_0": "sabbābharaṇabhūsito;",
  "thag1.117:1.3_0": "Tisso vijjā ajjhagamiṁ,",
  "thag1.117:1.4_0": "kataṁ buddhassa sāsanan”ti.",
  "thag1.117:1.5_0": "… Yaso thero …."
}