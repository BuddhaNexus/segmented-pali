{
  "ja204:0.1_0": "Jātaka",
  "ja204:0.2_0": "Dukanipāta",
  "ja204:0.3_0": "Nataṁdaḷhavagga",
  "ja204:0.4_0": "4. Vīrakajātaka",
  "ja204:1.1_0": "“Api vīraka passesi,",
  "ja204:1.2_0": "sakuṇaṁ mañjubhāṇakaṁ;",
  "ja204:1.3_0": "Mayūragīvasaṅkāsaṁ,",
  "ja204:1.4_0": "patiṁ mayhaṁ saviṭṭhakaṁ”.",
  "ja204:2.1_0": "“Udakathalacarassa pakkhino,",
  "ja204:2.2_0": "Niccaṁ āmakamacchabhojino;",
  "ja204:2.3_0": "Tassānukaraṁ saviṭṭhako,",
  "ja204:2.4_0": "Sevāle paliguṇṭhito mato”ti.",
  "ja204:3.1_0": "Vīrakajātakaṁ catutthaṁ."
}