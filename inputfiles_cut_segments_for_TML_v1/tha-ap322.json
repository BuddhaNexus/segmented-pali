{
  "tha-ap322:0.1_0": "Therāpadāna",
  "tha-ap322:0.2_0": "Ārakkhadāyakavagga",
  "tha-ap322:0.3_0": "10. Kumudamāliyattheraapadāna",
  "tha-ap322:1.1_0": "“Usabhaṁ pavaraṁ vīraṁ,",
  "tha-ap322:1.2_0": "mahesiṁ vijitāvinaṁ;",
  "tha-ap322:1.3_0": "Vipassinaṁ mahāvīraṁ,",
  "tha-ap322:1.4_0": "abhijātaṁva kesariṁ.",
  "tha-ap322:2.1_0": "Rathiyaṁ paṭipajjantaṁ,",
  "tha-ap322:2.2_0": "āhutīnaṁ paṭiggahaṁ;",
  "tha-ap322:2.3_0": "Gahetvā kumudaṁ mālaṁ,",
  "tha-ap322:2.4_0": "buddhaseṭṭhaṁ samokiriṁ.",
  "tha-ap322:3.1_0": "Ekanavutito kappe,",
  "tha-ap322:3.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap322:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap322:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap322:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap322:4.2_0": "…pe…",
  "tha-ap322:4.3_0": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap322:5.1_0": "Itthaṁ sudaṁ āyasmā kumudamāliyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap322:6.1_0": "Kumudamāliyattherassāpadānaṁ dasamaṁ.",
  "tha-ap322:7.1_0": "Ārakkhadāyakavaggo bāttiṁsatimo.",
  "tha-ap322:8.1_0": "Tassuddānaṁ",
  "tha-ap322:9.1_0": "Ārakkhado bhojanado,",
  "tha-ap322:9.2_0": "gatasaññī padumiyo;",
  "tha-ap322:9.3_0": "Pupphāsanī santhaviko,",
  "tha-ap322:9.4_0": "saddasaññī tiraṁsiyo;",
  "tha-ap322:9.5_0": "Kandaliko kumudī ca,",
  "tha-ap322:9.6_0": "sattapaññāsa gāthakāti."
}