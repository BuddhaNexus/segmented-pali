{
  "tha-ap193:0.1_0": "Therāpadāna",
  "tha-ap193:0.2_0": "Tamālapupphiyavagga",
  "tha-ap193:0.3_0": "1. Tamālapupphiyattheraapadāna",
  "tha-ap193:1.1_0": "“Cullāsītisahassāni,",
  "tha-ap193:1.2_0": "thambhā sovaṇṇayā ahū;",
  "tha-ap193:1.3_0": "Devalaṭṭhipaṭibhāgaṁ,",
  "tha-ap193:1.4_0": "vimānaṁ me sunimmitaṁ.",
  "tha-ap193:2.1_0": "Tamālapupphaṁ paggayha,",
  "tha-ap193:2.2_0": "vippasannena cetasā;",
  "tha-ap193:2.3_0": "Buddhassa abhiropayiṁ,",
  "tha-ap193:2.4_0": "sikhino lokabandhuno.",
  "tha-ap193:3.1_0": "Ekattiṁse ito kappe,",
  "tha-ap193:3.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap193:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap193:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap193:4.1_0": "Ito vīsatime kappe,",
  "tha-ap193:4.2_0": "candatittoti ekako;",
  "tha-ap193:4.3_0": "Sattaratanasampanno,",
  "tha-ap193:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap193:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap193:5.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap193:5.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap193:5.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap193:6.1_0": "Itthaṁ sudaṁ āyasmā tamālapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap193:7.1_0": "Tamālapupphiyattherassāpadānaṁ paṭhamaṁ."
}