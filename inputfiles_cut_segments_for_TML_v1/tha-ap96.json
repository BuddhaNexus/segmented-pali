{
  "tha-ap96:0.1_0": "Therāpadāna",
  "tha-ap96:0.2_0": "Sudhāvagga",
  "tha-ap96:0.3_0": "4. Sūcidāyakattheraapadāna",
  "tha-ap96:1.1_0": "“Kammārohaṁ pure āsiṁ,",
  "tha-ap96:1.2_0": "bandhumāyaṁ puruttame;",
  "tha-ap96:1.3_0": "Sūcidānaṁ mayā dinnaṁ,",
  "tha-ap96:1.4_0": "vipassissa mahesino.",
  "tha-ap96:2.1_0": "Vajiraggasamaṁ ñāṇaṁ,",
  "tha-ap96:2.2_0": "hoti kammena tādisaṁ;",
  "tha-ap96:2.3_0": "Virāgomhi vimuttomhi,",
  "tha-ap96:2.4_0": "pattomhi āsavakkhayaṁ.",
  "tha-ap96:3.1_0": "Atīte ca bhave sabbe,",
  "tha-ap96:3.2_0": "vattamāne canāgate;",
  "tha-ap96:3.3_0": "Ñāṇena viciniṁ sabbaṁ,",
  "tha-ap96:3.4_0": "sūcidānassidaṁ phalaṁ.",
  "tha-ap96:4.1_0": "Ekanavutito kappe,",
  "tha-ap96:4.2_0": "sattāsuṁ vajiravhayā;",
  "tha-ap96:4.3_0": "Sattaratanasampannā,",
  "tha-ap96:4.4_0": "cakkavattī mahabbalā.",
  "tha-ap96:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap96:5.2_0": "…pe…",
  "tha-ap96:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap96:6.1_0": "Itthaṁ sudaṁ āyasmā sūcidāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap96:7.1_0": "Sūcidāyakattherassāpadānaṁ catutthaṁ."
}