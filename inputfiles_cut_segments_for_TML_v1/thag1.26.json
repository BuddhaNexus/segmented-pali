{
  "thag1.26:0.1_0": "Theragāthā",
  "thag1.26:0.2_0": "Ekakanipāta",
  "thag1.26:0.3_0": "Tatiyavagga",
  "thag1.26:0.4_0": "1.26. Abhayattheragāthā",
  "thag1.26:1.1_0": "“Sutvā subhāsitaṁ vācaṁ,",
  "thag1.26:1.2_0": "buddhassādiccabandhuno;",
  "thag1.26:1.3_0": "Paccabyadhiṁ hi nipuṇaṁ,",
  "thag1.26:1.4_0": "vālaggaṁ usunā yathā”ti.",
  "thag1.26:1.5_0": "… Abhayo thero …."
}