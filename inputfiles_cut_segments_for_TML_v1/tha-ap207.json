{
  "tha-ap207:0.1_0": "Therāpadāna",
  "tha-ap207:0.2_0": "Kaṇikārapupphiyavagga",
  "tha-ap207:0.3_0": "5. Nigguṇḍipupphiyattheraapadāna",
  "tha-ap207:1.1_0": "“Vipassissa bhagavato,",
  "tha-ap207:1.2_0": "āsimārāmiko ahaṁ;",
  "tha-ap207:1.3_0": "Nigguṇḍipupphaṁ paggayha,",
  "tha-ap207:1.4_0": "buddhassa abhiropayiṁ.",
  "tha-ap207:2.1_0": "Ekanavutito kappe,",
  "tha-ap207:2.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap207:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap207:2.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap207:3.1_0": "Pañcavīse ito kappe,",
  "tha-ap207:3.2_0": "eko āsiṁ janādhipo;",
  "tha-ap207:3.3_0": "Mahāpatāpanāmena,",
  "tha-ap207:3.4_0": "cakkavattī mahabbalo.",
  "tha-ap207:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap207:4.2_0": "…pe…",
  "tha-ap207:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap207:5.1_0": "Itthaṁ sudaṁ āyasmā nigguṇḍipupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap207:6.1_0": "Nigguṇḍipupphiyattherassāpadānaṁ pañcamaṁ."
}