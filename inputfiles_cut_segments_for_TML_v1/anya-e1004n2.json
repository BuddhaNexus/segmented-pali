{
  "anya-e1004n2:0_0": "Mittakathā",
  "anya-e1004n2:1_0": "113.",
  "anya-e1004n2:2_0": "Katvāna kusalaṁkammaṁ,",
  "anya-e1004n2:3_0": "Katvācā kusalaṁ pure;",
  "anya-e1004n2:4_0": "Sukhitadukkhitā honti,",
  "anya-e1004n2:5_0": "So bālo yo napassati.",
  "anya-e1004n2:6_0": "114.",
  "anya-e1004n2:7_0": "Sayaṁkatena pāpena,",
  "anya-e1004n2:8_0": "Aniṭṭhaṁ labhate phalaṁ;",
  "anya-e1004n2:9_0": "Te me so me janentīti,",
  "anya-e1004n2:10_0": "Punāguṁ kurute jaḷo.",
  "anya-e1004n2:11_0": "115.",
  "anya-e1004n2:12_0": "Kālakhepena hāpeti,",
  "anya-e1004n2:13_0": "Dānasīlādikaṁ jaḷo;",
  "anya-e1004n2:14_0": "Athirampi thiraṁ maññe,",
  "anya-e1004n2:15_0": "Attānaṁ sassatīsamaṁ.",
  "anya-e1004n2:16_0": "116.",
  "anya-e1004n2:17_0": "Bālova pāpakaṁ katvā,",
  "anya-e1004n2:18_0": "Nataṁchaṭṭetumussahe;",
  "anya-e1004n2:19_0": "Kiṁ byagghatādi gacchanto,",
  "anya-e1004n2:20_0": "Padaṁ makkhetumussahe.",
  "anya-e1004n2:21_0": "117.",
  "anya-e1004n2:22_0": "Paranassanato naṭṭho,",
  "anya-e1004n2:23_0": "Pureva paranāsako;",
  "anya-e1004n2:24_0": "Sīghaṁvā dassanaṁ yāti,",
  "anya-e1004n2:25_0": "Tiṇaṁ pāsādajjhāpakaṁ.",
  "anya-e1004n2:26_0": "118.",
  "anya-e1004n2:27_0": "Bhojanā methunā niddā,",
  "anya-e1004n2:28_0": "Gave poseca vijjati;",
  "anya-e1004n2:29_0": "Vijjā viseso posassa,",
  "anya-e1004n2:30_0": "Tatohīno gosamo bhave.",
  "anya-e1004n2:31_0": "119.",
  "anya-e1004n2:32_0": "Muḷhasisso padesena,",
  "anya-e1004n2:33_0": "Kunārībharaṇenaca;",
  "anya-e1004n2:34_0": "Khalasatthūhisaṁyogā,",
  "anya-e1004n2:35_0": "Paṇḍitopyavasīdati.",
  "anya-e1004n2:36_0": "120.",
  "anya-e1004n2:37_0": "Dvecimekaṇṭakā tikkhā,",
  "anya-e1004n2:38_0": "Sarīrasobhitā kāme;",
  "anya-e1004n2:39_0": "Nidhano yoca kāmeti,",
  "anya-e1004n2:40_0": "Yocakuppatyanissāro.",
  "anya-e1004n2:41_0": "121.",
  "anya-e1004n2:42_0": "Nidhanocāpi kāmeti,",
  "anya-e1004n2:43_0": "Dubbalo kalahaṁpiyo;",
  "anya-e1004n2:44_0": "Mandasattho vivādatthi,",
  "anya-e1004n2:45_0": "Tividhaṁ muḷhalakkhaṇaṁ.",
  "anya-e1004n2:46_0": "122.",
  "anya-e1004n2:47_0": "Appasuto sute appe,",
  "anya-e1004n2:48_0": "Bahuṁmaññati mānavo;",
  "anya-e1004n2:49_0": "Sindhudakama passanto,",
  "anya-e1004n2:50_0": "Kūpetoyaṁva maṇḍuko.",
  "anya-e1004n2:51_0": "123.",
  "anya-e1004n2:52_0": "Tadamināpi jānāti,",
  "anya-e1004n2:53_0": "Sobbhesu padaresuca;",
  "anya-e1004n2:54_0": "Sunantā yanti kusumbhā,",
  "anya-e1004n2:55_0": "Tuṇhīyanti mahodadhi.",
  "anya-e1004n2:56_0": "124.",
  "anya-e1004n2:57_0": "Yaṁ ūnakaṁ taṁ sunati,",
  "anya-e1004n2:58_0": "Yaṁ pūraṁ santameva taṁ;",
  "anya-e1004n2:59_0": "Aḍḍhakumbhūpamo bālo,",
  "anya-e1004n2:60_0": "Rahato pūrova paṇḍito.",
  "anya-e1004n2:61_0": "125.",
  "anya-e1004n2:62_0": "Budhehibhāsamānopi ,",
  "anya-e1004n2:63_0": "Khalo bahutaketavo;",
  "anya-e1004n2:64_0": "Ghaṁsiyamāno aṅgāro,",
  "anya-e1004n2:65_0": "Nilamattaṁ nagacchati.",
  "anya-e1004n2:66_0": "126.",
  "anya-e1004n2:67_0": "Cārutā paradārāya,",
  "anya-e1004n2:68_0": "Dhanaṁ lokatappatiyā;",
  "anya-e1004n2:69_0": "Pasutaṁ sādhunāsāya,",
  "anya-e1004n2:70_0": "Khale khalatarā guṇā.",
  "anya-e1004n2:71_0": "127.",
  "anya-e1004n2:72_0": "Ito hassataraṁ loke,",
  "anya-e1004n2:73_0": "Kiñci tassa navijjati;",
  "anya-e1004n2:74_0": "Dujjanotica yaṁāha,",
  "anya-e1004n2:75_0": "Sajjanaṁ dujjano sayaṁ.",
  "anya-e1004n2:76_0": "128.",
  "anya-e1004n2:77_0": "Rogaṇḍacaṅkuro tejo,",
  "anya-e1004n2:78_0": "Visamassataro ghaṇo;",
  "anya-e1004n2:79_0": "Avināsiya samanti,",
  "anya-e1004n2:80_0": "Na khaloca sanissayaṁ.",
  "anya-e1004n2:81_0": "129.",
  "anya-e1004n2:82_0": "Navinā paravādena,",
  "anya-e1004n2:83_0": "Ramante dujjanā khalu;",
  "anya-e1004n2:84_0": "Na sā sabbarase bhutvā,",
  "anya-e1004n2:85_0": "Vinā vacce natussati.",
  "anya-e1004n2:86_0": "130.",
  "anya-e1004n2:87_0": "Tappate yāti sandhānaṁ,",
  "anya-e1004n2:88_0": "Davo bhavati vanataṁ;",
  "anya-e1004n2:89_0": "Muduṁ dujjanacittaṁ kiṁ,",
  "anya-e1004n2:90_0": "Lohena upamīyate.",
  "anya-e1004n2:91_0": "131.",
  "anya-e1004n2:92_0": "Tasmā dujjanasaṁsaggaṁ,",
  "anya-e1004n2:93_0": "Āsīvisa mivoragaṁ;",
  "anya-e1004n2:94_0": "Ārakā parivajjeyya,",
  "anya-e1004n2:95_0": "Bhūtikāmo vicakkhaṇo.",
  "anya-e1004n2:96_0": "132.",
  "anya-e1004n2:97_0": "Dujjanena hi saṁsaggo,",
  "anya-e1004n2:98_0": "Sattutāpi nayujjate;",
  "anya-e1004n2:99_0": "Tatto dahati aṅgāro,",
  "anya-e1004n2:100_0": "Santātu kālataṁkaro.",
  "anya-e1004n2:101_0": "133.",
  "anya-e1004n2:102_0": "Dujjano vajjanīyova,",
  "anya-e1004n2:103_0": "Vijjāyālaṅkatopi ce;",
  "anya-e1004n2:104_0": "Maṇinā laṅkato santo,",
  "anya-e1004n2:105_0": "Sappo kiṁ nabhayaṅkaro.",
  "anya-e1004n2:106_0": "134.",
  "anya-e1004n2:107_0": "Aggino dahato dāyaṁ,",
  "anya-e1004n2:108_0": "Sakhābhavati māluto;",
  "anya-e1004n2:109_0": "Sova dīpaṁtu nāseti,",
  "anya-e1004n2:110_0": "Khale nattheva mittatā.",
  "anya-e1004n2:111_0": "135.",
  "anya-e1004n2:112_0": "Nasmase katapāpamhi,",
  "anya-e1004n2:113_0": "Nasmase alikavādini;",
  "anya-e1004n2:114_0": "Nasmasettatthapaññamhi,",
  "anya-e1004n2:115_0": "Atisantepi nasmase.",
  "anya-e1004n2:116_0": "136.",
  "anya-e1004n2:117_0": "Māretuṁ kittakā sakkā,",
  "anya-e1004n2:118_0": "Dujjanā gagaṇūpamā;",
  "anya-e1004n2:119_0": "Māritā kodhacittetu,",
  "anya-e1004n2:120_0": "Mārito honti dujjanā.",
  "anya-e1004n2:121_0": "137.",
  "anya-e1004n2:122_0": "Bhūmi kaṇṭakasaṁkiṇṇā,",
  "anya-e1004n2:123_0": "Chādituṁ nevasakyate;",
  "anya-e1004n2:124_0": "Upāhana mattakena,",
  "anya-e1004n2:125_0": "Channā bhavati medanī.",
  "anya-e1004n2:126_0": "138.",
  "anya-e1004n2:127_0": "Sattā sadāpasevanti,",
  "anya-e1004n2:128_0": "Sodakaṁ vāpiādikaṁ;",
  "anya-e1004n2:129_0": "Sabhogaṁ sadhanañcevaṁ,",
  "anya-e1004n2:130_0": "Tucchāce te jahanti te.",
  "anya-e1004n2:131_0": "139.",
  "anya-e1004n2:132_0": "Dhanahīnaṁ caje mitto,",
  "anya-e1004n2:133_0": "Puttadārā sahodarā;",
  "anya-e1004n2:134_0": "Atthavantaṁva sevanti,",
  "anya-e1004n2:135_0": "Attho loke mahāsakhā.",
  "anya-e1004n2:136_0": "140.",
  "anya-e1004n2:137_0": "Na rūpinī na paññāṇo,",
  "anya-e1004n2:138_0": "Nakulīno padhānatā;",
  "anya-e1004n2:139_0": "Kāle vipattisampatte,",
  "anya-e1004n2:140_0": "Dhanimāva visesatā.",
  "anya-e1004n2:141_0": "141.",
  "anya-e1004n2:142_0": "Kalyāṇamittaṁ kantāraṁ,",
  "anya-e1004n2:143_0": "Yuddhaṁ sabhāyaṁ bhāsituṁ;",
  "anya-e1004n2:144_0": "Asatthā gantu micchanti,",
  "anya-e1004n2:145_0": "Muḷhā tecaturojanā.",
  "anya-e1004n2:146_0": "142.",
  "anya-e1004n2:147_0": "Ahitā paṭisedhoca,",
  "anya-e1004n2:148_0": "Hitesuca payojanaṁ;",
  "anya-e1004n2:149_0": "Byasanesva pariccāgo,",
  "anya-e1004n2:150_0": "Saṅkhepā mittalakkhaṇaṁ.",
  "anya-e1004n2:151_0": "143.",
  "anya-e1004n2:152_0": "Āture byasanepatte,",
  "anya-e1004n2:153_0": "Dubbhikkhe sattu viggahe;",
  "anya-e1004n2:154_0": "Rājadvāre susāneca,",
  "anya-e1004n2:155_0": "Yo tiṭṭhati sabandhavo.",
  "anya-e1004n2:156_0": "144.",
  "anya-e1004n2:157_0": "So bandhu yo hiteyutto,",
  "anya-e1004n2:158_0": "So pitā yo tu posako;",
  "anya-e1004n2:159_0": "So ñāti yatra visāso,",
  "anya-e1004n2:160_0": "Sā bhariyā yatra nibbūti.",
  "anya-e1004n2:161_0": "145.",
  "anya-e1004n2:162_0": "Hitesino sumittoca,",
  "anya-e1004n2:163_0": "Viññūca dullabhā janā;",
  "anya-e1004n2:164_0": "Yatho sadhaṁca sāduṁca,",
  "anya-e1004n2:165_0": "Rogahārīca sujano.",
  "anya-e1004n2:166_0": "146.",
  "anya-e1004n2:167_0": "Agaruko anālasso,",
  "anya-e1004n2:168_0": "Asaṭṭhoca saccavā suci;",
  "anya-e1004n2:169_0": "Aluddho atthakāmoca,",
  "anya-e1004n2:170_0": "Ediso suhaduttamo.",
  "anya-e1004n2:171_0": "147.",
  "anya-e1004n2:172_0": "Yo dhuvāni pariccajja,",
  "anya-e1004n2:173_0": "Adhūvānyupasevati;",
  "anya-e1004n2:174_0": "Dhuvāpi tassa nassanti,",
  "anya-e1004n2:175_0": "Adhuvesu kathāvakā.",
  "anya-e1004n2:176_0": "148.",
  "anya-e1004n2:177_0": "Luddhamatthenagaṇheyya ,",
  "anya-e1004n2:178_0": "Thaddha mañjalikammunā;",
  "anya-e1004n2:179_0": "Chandānuvattiyā muḷhaṁ,",
  "anya-e1004n2:180_0": "Yathābhūtena paṇḍitaṁ.",
  "anya-e1004n2:181_0": "149.",
  "anya-e1004n2:182_0": "Uttamaṁ pāṇipātena,",
  "anya-e1004n2:183_0": "Sūraṁ bhedena vijaye;",
  "anya-e1004n2:184_0": "Nicaṁ dabbapadānena,",
  "anya-e1004n2:185_0": "Vikkamena samaṁ jaye.",
  "anya-e1004n2:186_0": "150.",
  "anya-e1004n2:187_0": "Yassayassa hi yobhāvo,",
  "anya-e1004n2:188_0": "Tenatena hi taṁnaraṁ;",
  "anya-e1004n2:189_0": "Anupavissa medhāvī,",
  "anya-e1004n2:190_0": "Khippamattavasaṁ naye.",
  "anya-e1004n2:191_0": "151.",
  "anya-e1004n2:192_0": "Yena icchati sambandhaṁ,",
  "anya-e1004n2:193_0": "Tena tīṇi nakāraye;",
  "anya-e1004n2:194_0": "Vivāda matthasambandhaṁ,",
  "anya-e1004n2:195_0": "Parokkhe dāradassanaṁ.",
  "anya-e1004n2:196_0": "152.",
  "anya-e1004n2:197_0": "Accābhikkhaṇasaṁsaggā,",
  "anya-e1004n2:198_0": "Asammosaraṇenaca;",
  "anya-e1004n2:199_0": "Etena mittā jīranti,",
  "anya-e1004n2:200_0": "Akāle yācanāyaca.",
  "anya-e1004n2:201_0": "153.",
  "anya-e1004n2:202_0": "Tasmā nābhikkhaṇaṁ gacche,",
  "anya-e1004n2:203_0": "Nacagacche cirāciraṁ;",
  "anya-e1004n2:204_0": "Kālena yācaṁ yāceyya,",
  "anya-e1004n2:205_0": "Evaṁ mittā najīraye.",
  "anya-e1004n2:206_0": "154.",
  "anya-e1004n2:207_0": "Ete bhiyyo samaṁ yanti,",
  "anya-e1004n2:208_0": "Sandhi tesaṁ najīrati;",
  "anya-e1004n2:209_0": "Yo adhippannaṁ sahati,",
  "anya-e1004n2:210_0": "Yoca jānāti desanaṁ.",
  "anya-e1004n2:211_0": "155.",
  "anya-e1004n2:212_0": "Sace santā vivādanti,",
  "anya-e1004n2:213_0": "Khippaṁ sandhiraye puna;",
  "anya-e1004n2:214_0": "Bālā pattāva bhijjanti,",
  "anya-e1004n2:215_0": "Na te samathamajjhaguṁ.",
  "anya-e1004n2:216_0": "156.",
  "anya-e1004n2:217_0": "Yenamittena saṁsaggo,",
  "anya-e1004n2:218_0": "Yogakkhemo vihīyati;",
  "anya-e1004n2:219_0": "Pubbeva jjhābhavantassa,",
  "anya-e1004n2:220_0": "Rakkhe akkhiva paṇḍito.",
  "anya-e1004n2:221_0": "157.",
  "anya-e1004n2:222_0": "Yenamittena saṁsaggā,",
  "anya-e1004n2:223_0": "Yogakkhemo pavaḍḍhati;",
  "anya-e1004n2:224_0": "Kareyyattasamaṁ vutti,",
  "anya-e1004n2:225_0": "Sabbakiccesu paṇḍito.",
  "anya-e1004n2:226_0": "158.",
  "anya-e1004n2:227_0": "Guṇo sabbaññutulyopi,",
  "anya-e1004n2:228_0": "Sīdate ko anissayo;",
  "anya-e1004n2:229_0": "Anaggha mapimāṇikkaṁ,",
  "anya-e1004n2:230_0": "Hemaṁ nissāya sobhate.",
  "anya-e1004n2:231_0": "159.",
  "anya-e1004n2:232_0": "Pabbepabbe kamenucchu,",
  "anya-e1004n2:233_0": "Visesarasivaggato;",
  "anya-e1004n2:234_0": "Tathā sumittiko sādhu,",
  "anya-e1004n2:235_0": "Viparitova dujjano.",
  "anya-e1004n2:236_0": "160.",
  "anya-e1004n2:237_0": "Teneva muninā vuttaṁ,",
  "anya-e1004n2:238_0": "Yekecilokiyādhammā;",
  "anya-e1004n2:239_0": "Tathā nibbānagāmīno,",
  "anya-e1004n2:240_0": "Santi lokuttarādhammā.",
  "anya-e1004n2:241_0": "161.",
  "anya-e1004n2:242_0": "Kalyāṇamitta 1 māgamma,",
  "anya-e1004n2:243_0": "Sabbe te honti pāṇinaṁ;",
  "anya-e1004n2:244_0": "Tasmā kalyāṇamittesu,",
  "anya-e1004n2:245_0": "Kātabbo ādaro sadā.",
  "anya-e1004n2:246_0": "162.",
  "anya-e1004n2:247_0": "Yo ve kataññū katavediko dhiro,",
  "anya-e1004n2:248_0": "Kalyāṇamitto daḷhabhattica hoti;",
  "anya-e1004n2:249_0": "Dukkhitassa sakkaccaṁ kareti kiccaṁ,",
  "anya-e1004n2:250_0": "Tathāvidhaṁ sappurisāti vadanti.",
  "anya-e1004n2:251_0": "163.",
  "anya-e1004n2:252_0": "Yassahi dhammaṁ puriso vijaññā,",
  "anya-e1004n2:253_0": "Yecassa kaṅkhaṁ vinayanti santo;",
  "anya-e1004n2:254_0": "Tañhissa dīpañca parāyanañca,",
  "anya-e1004n2:255_0": "Na tena mittaṁ jirayetha pañño."
}