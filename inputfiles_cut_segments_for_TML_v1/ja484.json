{
  "ja484:0.1_0": "Jātaka",
  "ja484:0.2_0": "Pakiṇṇakanipāta",
  "ja484:0.3_0": "Sālikedāravagga",
  "ja484:0.4_0": "1. Sālikedārajātaka",
  "ja484:1.1_0": "“Sampannaṁ sālikedāraṁ,",
  "ja484:1.2_0": "suvā bhuñjanti kosiya;",
  "ja484:1.3_0": "Paṭivedemi te brahme,",
  "ja484:1.4_0": "na ne vāretumussahe.",
  "ja484:2.1_0": "Eko ca tattha sakuṇo,",
  "ja484:2.2_0": "yo nesaṁ sabbasundaro;",
  "ja484:2.3_0": "Bhutvā sāliṁ yathākāmaṁ,",
  "ja484:2.4_0": "tuṇḍenādāya gacchati”.",
  "ja484:3.1_0": "“Oḍḍentu vāḷapāsāni,",
  "ja484:3.2_0": "yathā bajjhetha so dijo;",
  "ja484:3.3_0": "Jīvañca naṁ gahetvāna,",
  "ja484:3.4_0": "ānayehi mamantike”.",
  "ja484:4.1_0": "“Ete bhutvā pivitvā ca,",
  "ja484:4.2_0": "pakkamanti vihaṅgamā;",
  "ja484:4.3_0": "Eko baddhosmi pāsena,",
  "ja484:4.4_0": "kiṁ pāpaṁ pakataṁ mayā”.",
  "ja484:5.1_0": "“Udaraṁ nūna aññesaṁ,",
  "ja484:5.2_0": "suva accodaraṁ tava;",
  "ja484:5.3_0": "Bhutvā sāliṁ yathākāmaṁ,",
  "ja484:5.4_0": "tuṇḍenādāya gacchasi.",
  "ja484:6.1_0": "Koṭṭhaṁ nu tattha pūresi,",
  "ja484:6.2_0": "suva veraṁ nu te mayā;",
  "ja484:6.3_0": "Puṭṭho me samma akkhāhi,",
  "ja484:6.4_0": "kuhiṁ sāliṁ nidāhasi”.",
  "ja484:7.1_0": "“Na me veraṁ tayā saddhiṁ,",
  "ja484:7.2_0": "koṭṭho mayhaṁ na vijjati;",
  "ja484:7.3_0": "Iṇaṁ muñcāmiṇaṁ dammi,",
  "ja484:7.4_0": "sampatto koṭasimbaliṁ;",
  "ja484:7.5_0": "Nidhimpi tattha nidahāmi,",
  "ja484:7.6_0": "evaṁ jānāhi kosiya”.",
  "ja484:8.1_0": "“Kīdisaṁ te iṇadānaṁ,",
  "ja484:8.2_0": "iṇamokkho ca kīdiso;",
  "ja484:8.3_0": "Nidhinidhānamakkhāhi,",
  "ja484:8.4_0": "atha pāsā pamokkhasi”.",
  "ja484:9.1_0": "“Ajātapakkhā taruṇā,",
  "ja484:9.2_0": "puttakā mayha kosiya;",
  "ja484:9.3_0": "Te maṁ bhatā bharissanti,",
  "ja484:9.4_0": "tasmā tesaṁ iṇaṁ dade.",
  "ja484:10.1_0": "Mātā pitā ca me vuddhā,",
  "ja484:10.2_0": "jiṇṇakā gatayobbanā;",
  "ja484:10.3_0": "Tesaṁ tuṇḍena hātūna,",
  "ja484:10.4_0": "muñce pubbakataṁ iṇaṁ.",
  "ja484:11.1_0": "Aññepi tattha sakuṇā,",
  "ja484:11.2_0": "khīṇapakkhā sudubbalā;",
  "ja484:11.3_0": "Tesaṁ puññatthiko dammi,",
  "ja484:11.4_0": "taṁ nidhiṁ āhu paṇḍitā.",
  "ja484:12.1_0": "Īdisaṁ me iṇadānaṁ,",
  "ja484:12.2_0": "iṇamokkho ca īdiso;",
  "ja484:12.3_0": "Nidhinidhānamakkhāmi,",
  "ja484:12.4_0": "evaṁ jānāhi kosiya”.",
  "ja484:13.1_0": "“Bhaddako vatayaṁ pakkhī,",
  "ja484:13.2_0": "dijo paramadhammiko;",
  "ja484:13.3_0": "Ekaccesu manussesu,",
  "ja484:13.4_0": "ayaṁ dhammo na vijjati.",
  "ja484:14.1_0": "Bhuñja sāliṁ yathākāmaṁ,",
  "ja484:14.2_0": "saha sabbehi ñātibhi;",
  "ja484:14.3_0": "Punāpi suva passemu,",
  "ja484:14.4_0": "piyaṁ me tava dassanaṁ”.",
  "ja484:15.1_0": "“Bhuttañca pītañca tavassamamhi,",
  "ja484:15.2_0": "Ratī ca no kosiya te sakāse;",
  "ja484:15.3_0": "Nikkhittadaṇḍesu dadāhi dānaṁ,",
  "ja484:15.4_0": "Jiṇṇe ca mātāpitaro bharassu”.",
  "ja484:16.1_0": "“Lakkhī vata me udapādi ajja,",
  "ja484:16.2_0": "Yo addasāsiṁ pavaraṁ dijānaṁ;",
  "ja484:16.3_0": "Suvassa sutvāna subhāsitāni,",
  "ja484:16.4_0": "Kāhāmi puññāni anappakāni”.",
  "ja484:17.1_0": "So kosiyo attamano udaggo,",
  "ja484:17.2_0": "Annañca pānañcabhisaṅkharitvā;",
  "ja484:17.3_0": "Annena pānena pasannacitto,",
  "ja484:17.4_0": "Santappayi samaṇabrāhmaṇe cāti.",
  "ja484:18.1_0": "Sālikedārajātakaṁ paṭhamaṁ."
}