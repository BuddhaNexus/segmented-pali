{
  "tha-ap137:0.1_0": "Therāpadāna",
  "tha-ap137:0.2_0": "Sobhitavagga",
  "tha-ap137:0.3_0": "5 Rahosaññakattheraapadāna",
  "tha-ap137:1.1_0": "“Himavantassāvidūre,",
  "tha-ap137:1.2_0": "vasabho nāma pabbato;",
  "tha-ap137:1.3_0": "Tasmiṁ pabbatapādamhi,",
  "tha-ap137:1.4_0": "assamo āsi māpito.",
  "tha-ap137:2.1_0": "Tīṇi sissasahassāni,",
  "tha-ap137:2.2_0": "vācesiṁ brāhmaṇo tadā;",
  "tha-ap137:2.3_0": "Saṁharitvāna te sisse,",
  "tha-ap137:2.4_0": "ekamantaṁ upāvisiṁ.",
  "tha-ap137:3.1_0": "Ekamantaṁ nisīditvā,",
  "tha-ap137:3.2_0": "brāhmaṇo mantapāragū;",
  "tha-ap137:3.3_0": "Buddhavedaṁ gavesanto,",
  "tha-ap137:3.4_0": "ñāṇe cittaṁ pasādayiṁ.",
  "tha-ap137:4.1_0": "Tattha cittaṁ pasādetvā,",
  "tha-ap137:4.2_0": "nisīdiṁ paṇṇasanthare;",
  "tha-ap137:4.3_0": "Pallaṅkaṁ ābhujitvāna,",
  "tha-ap137:4.4_0": "tattha kālaṅkato ahaṁ.",
  "tha-ap137:5.1_0": "Ekattiṁse ito kappe,",
  "tha-ap137:5.2_0": "yaṁ saññamalabhiṁ tadā;",
  "tha-ap137:5.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap137:5.4_0": "ñāṇasaññāyidaṁ phalaṁ.",
  "tha-ap137:6.1_0": "Sattavīsatikappamhi,",
  "tha-ap137:6.2_0": "rājā siridharo ahu;",
  "tha-ap137:6.3_0": "Sattaratanasampanno,",
  "tha-ap137:6.4_0": "cakkavattī mahabbalo.",
  "tha-ap137:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap137:7.2_0": "…pe…",
  "tha-ap137:7.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap137:8.1_0": "Itthaṁ sudaṁ āyasmā rahosaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap137:9.1_0": "Rahosaññakattherassāpadānaṁ pañcamaṁ."
}