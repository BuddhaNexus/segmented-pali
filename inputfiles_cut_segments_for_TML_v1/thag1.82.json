{
  "thag1.82:0.1_0": "Theragāthā",
  "thag1.82:0.2_0": "Ekakanipāta",
  "thag1.82:0.3_0": "Navamavagga",
  "thag1.82:0.4_0": "1.82. Kassapattheragāthā",
  "thag1.82:1.1_0": "“Yena yena subhikkhāni,",
  "thag1.82:1.2_0": "sivāni abhayāni ca;",
  "thag1.82:1.3_0": "Tena puttaka gacchassu,",
  "thag1.82:1.4_0": "mā sokāpahato bhavā”ti.",
  "thag1.82:1.5_0": "… Kassapo thero …."
}