{
  "pli-tv-bu-vb-as1:0.1_0": "Theravāda Vinaya",
  "pli-tv-bu-vb-as1:0.2_0": "Mahāvibhaṅga",
  "pli-tv-bu-vb-as1:0.3_0": "Adhikaraṇasamatha",
  "pli-tv-bu-vb-as1:1.1_0": "Ime kho panāyasmanto satta adhikaraṇasamathā dhammā uddesaṁ āgacchanti.",
  "pli-tv-bu-vb-as1:2.1_0": "Uppannuppannānaṁ adhikaraṇānaṁ samathāya vūpasamāya",
  "pli-tv-bu-vb-as1:2.2_0": "sammukhāvinayo dātabbo,",
  "pli-tv-bu-vb-as2:1.1_0": "sativinayo dātabbo,",
  "pli-tv-bu-vb-as3:1.1_0": "amūḷhavinayo dātabbo,",
  "pli-tv-bu-vb-as4:1.1_0": "paṭiññāya kāretabbaṁ,",
  "pli-tv-bu-vb-as5:1.1_0": "yebhuyyasikā,",
  "pli-tv-bu-vb-as6:1.1_0": "tassapāpiyasikā,",
  "pli-tv-bu-vb-as7:1.1_0": "tiṇavatthārakoti.",
  "pli-tv-bu-vb-as7:2.1_0": "Uddiṭṭhā kho, āyasmanto, satta adhikaraṇasamathā dhammā.",
  "pli-tv-bu-vb-as7:2.2_0": "Tatthāyasmante pucchāmi—",
  "pli-tv-bu-vb-as7:2.3_0": "“kaccittha parisuddhā”?",
  "pli-tv-bu-vb-as7:2.4_0": "Dutiyampi pucchāmi—",
  "pli-tv-bu-vb-as7:2.5_0": "“kaccittha parisuddhā”?",
  "pli-tv-bu-vb-as7:2.6_0": "Tatiyampi pucchāmi—",
  "pli-tv-bu-vb-as7:2.7_0": "“kaccittha parisuddhā”?",
  "pli-tv-bu-vb-as7:2.8_0": "Parisuddhetthāyasmanto, tasmā tuṇhī, evametaṁ dhārayāmīti.",
  "pli-tv-bu-vb-as7:3.1_0": "Adhikaraṇasamathā niṭṭhitā.",
  "pli-tv-bu-vb-as7:4.1_0": "Uddiṭṭhaṁ kho, āyasmanto, nidānaṁ;",
  "pli-tv-bu-vb-as7:4.2_0": "uddiṭṭhā cattāro pārājikā dhammā;",
  "pli-tv-bu-vb-as7:4.3_0": "uddiṭṭhā terasa saṅghādisesā dhammā;",
  "pli-tv-bu-vb-as7:4.4_0": "uddiṭṭhā dve aniyatā dhammā;",
  "pli-tv-bu-vb-as7:4.5_0": "uddiṭṭhā tiṁsa nissaggiyā pācittiyā dhammā;",
  "pli-tv-bu-vb-as7:4.6_0": "uddiṭṭhā dvenavuti pācittiyā dhammā;",
  "pli-tv-bu-vb-as7:4.7_0": "uddiṭṭhā cattāro pāṭidesanīyā dhammā;",
  "pli-tv-bu-vb-as7:4.8_0": "uddiṭṭhā sekhiyā dhammā;",
  "pli-tv-bu-vb-as7:4.9_0": "uddiṭṭhā satta adhikaraṇasamathā dhammā.",
  "pli-tv-bu-vb-as7:4.10_0": "Ettakaṁ tassa bhagavato suttāgataṁ suttapariyāpannaṁ anvaddhamāsaṁ uddesaṁ āgacchati.",
  "pli-tv-bu-vb-as7:4.11_0": "Tattha sabbeheva samaggehi sammodamānehi avivadamānehi sikkhitabbanti.",
  "pli-tv-bu-vb-as7:5.1_0": "Mahāvibhaṅgo niṭṭhito."
}