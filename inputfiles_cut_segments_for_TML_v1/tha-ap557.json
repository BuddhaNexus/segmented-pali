{
  "tha-ap557:0.1_0": "Therāpadāna",
  "tha-ap557:0.2_0": "Yasavagga",
  "tha-ap557:0.3_0": "5. Vajjīputtattheraapadāna",
  "tha-ap557:1.1_0": "“Sahassaraṁsī bhagavā,",
  "tha-ap557:1.2_0": "sayambhū aparājito;",
  "tha-ap557:1.3_0": "Vivekā vuṭṭhahitvāna,",
  "tha-ap557:1.4_0": "gocarāyābhinikkhami.",
  "tha-ap557:2.1_0": "Phalahattho ahaṁ disvā,",
  "tha-ap557:2.2_0": "upagacchiṁ narāsabhaṁ;",
  "tha-ap557:2.3_0": "Pasannacitto sumano,",
  "tha-ap557:2.4_0": "savaṇṭaṁ adadiṁ phalaṁ.",
  "tha-ap557:3.1_0": "Catunnavutito kappe,",
  "tha-ap557:3.2_0": "yaṁ phalaṁ adadiṁ tadā;",
  "tha-ap557:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap557:3.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap557:4.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap557:4.2_0": "…pe…",
  "tha-ap557:4.3_0": "viharāmi anāsavo.",
  "tha-ap557:5.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap557:5.2_0": "…pe…",
  "tha-ap557:5.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap557:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap557:6.2_0": "…pe…",
  "tha-ap557:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap557:7.1_0": "Itthaṁ sudaṁ āyasmā vajjīputto thero imā gāthāyo abhāsitthāti.",
  "tha-ap557:8.1_0": "Vajjīputtattherassāpadānaṁ pañcamaṁ."
}