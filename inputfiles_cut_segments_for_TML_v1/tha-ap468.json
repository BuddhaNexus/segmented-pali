{
  "tha-ap468:0.1_0": "Therāpadāna",
  "tha-ap468:0.2_0": "Sālakusumiyavagga",
  "tha-ap468:0.3_0": "6 Avaṭaphaliyattheraapadāna",
  "tha-ap468:1.1_0": "“Sataraṁsī nāma bhagavā,",
  "tha-ap468:1.2_0": "sayambhū aparājito;",
  "tha-ap468:1.3_0": "Vivekakāmo sambuddho,",
  "tha-ap468:1.4_0": "gocarāyābhinikkhami.",
  "tha-ap468:2.1_0": "Phalahattho ahaṁ disvā,",
  "tha-ap468:2.2_0": "upagacchiṁ narāsabhaṁ;",
  "tha-ap468:2.3_0": "Pasannacitto sumano,",
  "tha-ap468:2.4_0": "adāsiṁ avaṭaṁ phalaṁ.",
  "tha-ap468:3.1_0": "Catunnavutito kappe,",
  "tha-ap468:3.2_0": "yaṁ phalaṁ adadiṁ tadā;",
  "tha-ap468:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap468:3.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap468:4.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap468:4.2_0": "…pe…",
  "tha-ap468:4.3_0": "viharāmi anāsavo.",
  "tha-ap468:5.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap468:5.2_0": "…pe…",
  "tha-ap468:5.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap468:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap468:6.2_0": "…pe…",
  "tha-ap468:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap468:7.1_0": "Itthaṁ sudaṁ āyasmā avaṭaphaliyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap468:8.1_0": "Avaṭaphaliyattherassāpadānaṁ chaṭṭhaṁ."
}