{
  "tha-ap541:0.1_0": "Therāpadāna",
  "tha-ap541:0.2_0": "Kaccāyanavagga",
  "tha-ap541:0.3_0": "9. Rādhattheraapadāna",
  "tha-ap541:1.1_0": "“Padumuttaro nāma jino,",
  "tha-ap541:1.2_0": "sabbalokavidū muni;",
  "tha-ap541:1.3_0": "Ito satasahassamhi,",
  "tha-ap541:1.4_0": "kappe uppajji cakkhumā.",
  "tha-ap541:2.1_0": "Ovādako viññāpako,",
  "tha-ap541:2.2_0": "tārako sabbapāṇinaṁ;",
  "tha-ap541:2.3_0": "Desanākusalo buddho,",
  "tha-ap541:2.4_0": "tāresi janataṁ bahuṁ.",
  "tha-ap541:3.1_0": "Anukampako kāruṇiko,",
  "tha-ap541:3.2_0": "hitesī sabbapāṇinaṁ;",
  "tha-ap541:3.3_0": "Sampatte titthiye sabbe,",
  "tha-ap541:3.4_0": "pañcasīle patiṭṭhapi.",
  "tha-ap541:4.1_0": "Evaṁ nirākulaṁ āsi,",
  "tha-ap541:4.2_0": "suññataṁ titthiyehi ca;",
  "tha-ap541:4.3_0": "Vicittaṁ arahantehi,",
  "tha-ap541:4.4_0": "vasībhūtehi tādibhi.",
  "tha-ap541:5.1_0": "Ratanānaṭṭhapaññāsaṁ,",
  "tha-ap541:5.2_0": "uggato so mahāmuni;",
  "tha-ap541:5.3_0": "Kañcanagghiyasaṅkāso,",
  "tha-ap541:5.4_0": "bāttiṁsavaralakkhaṇo.",
  "tha-ap541:6.1_0": "Vassasatasahassāni,",
  "tha-ap541:6.2_0": "āyu vijjati tāvade;",
  "tha-ap541:6.3_0": "Tāvatā tiṭṭhamāno so,",
  "tha-ap541:6.4_0": "tāresi janataṁ bahuṁ.",
  "tha-ap541:7.1_0": "Tadāhaṁ haṁsavatiyaṁ,",
  "tha-ap541:7.2_0": "brāhmaṇo mantapāragū;",
  "tha-ap541:7.3_0": "Upecca taṁ naravaraṁ,",
  "tha-ap541:7.4_0": "assosiṁ dhammadesanaṁ.",
  "tha-ap541:8.1_0": "Paññāpentaṁ mahāvīraṁ,",
  "tha-ap541:8.2_0": "parisāsu visāradaṁ;",
  "tha-ap541:8.3_0": "Paṭibhāneyyakaṁ bhikkhuṁ,",
  "tha-ap541:8.4_0": "etadagge vināyakaṁ.",
  "tha-ap541:9.1_0": "Tadāhaṁ kāraṁ katvāna,",
  "tha-ap541:9.2_0": "sasaṅghe lokanāyake;",
  "tha-ap541:9.3_0": "Nipacca sirasā pāde,",
  "tha-ap541:9.4_0": "taṁ ṭhānaṁ abhipatthayiṁ.",
  "tha-ap541:10.1_0": "Tato maṁ bhagavā āha,",
  "tha-ap541:10.2_0": "siṅgīnikkhasamappabho;",
  "tha-ap541:10.3_0": "Sarena rajanīyena,",
  "tha-ap541:10.4_0": "kilesamalahārinā.",
  "tha-ap541:11.1_0": "‘Sukhī bhavassu dīghāyu,",
  "tha-ap541:11.2_0": "sijjhatu paṇidhī tava;",
  "tha-ap541:11.3_0": "Sasaṅghe me kataṁ kāraṁ,",
  "tha-ap541:11.4_0": "atīva vipulaṁ tayā.",
  "tha-ap541:12.1_0": "Satasahassito kappe,",
  "tha-ap541:12.2_0": "okkākakulasambhavo;",
  "tha-ap541:12.3_0": "Gotamo nāma gottena,",
  "tha-ap541:12.4_0": "satthā loke bhavissati.",
  "tha-ap541:13.1_0": "Tassa dhammesu dāyādo,",
  "tha-ap541:13.2_0": "oraso dhammanimmito;",
  "tha-ap541:13.3_0": "Rādhoti nāmadheyyena,",
  "tha-ap541:13.4_0": "hessati satthu sāvako.",
  "tha-ap541:14.1_0": "Sa te hetuguṇe tuṭṭho,",
  "tha-ap541:14.2_0": "sakyaputto narāsabho;",
  "tha-ap541:14.3_0": "Paṭibhāneyyakānaggaṁ,",
  "tha-ap541:14.4_0": "paññapessati nāyako’.",
  "tha-ap541:15.1_0": "Taṁ sutvā mudito hutvā,",
  "tha-ap541:15.2_0": "yāvajīvaṁ tadā jinaṁ;",
  "tha-ap541:15.3_0": "Mettacitto paricariṁ,",
  "tha-ap541:15.4_0": "sato paññāsamāhito.",
  "tha-ap541:16.1_0": "Tena kammena sukatena,",
  "tha-ap541:16.2_0": "cetanāpaṇidhīhi ca;",
  "tha-ap541:16.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "tha-ap541:16.4_0": "tāvatiṁsamagacchahaṁ.",
  "tha-ap541:17.1_0": "Satānaṁ tīṇikkhattuñca,",
  "tha-ap541:17.2_0": "devarajjamakārayiṁ;",
  "tha-ap541:17.3_0": "Satānaṁ pañcakkhattuñca,",
  "tha-ap541:17.4_0": "cakkavattī ahosahaṁ.",
  "tha-ap541:18.1_0": "Padesarajjaṁ vipulaṁ,",
  "tha-ap541:18.2_0": "gaṇanāto asaṅkhiyaṁ;",
  "tha-ap541:18.3_0": "Sabbattha sukhito āsiṁ,",
  "tha-ap541:18.4_0": "tassa kammassa vāhasā.",
  "tha-ap541:19.1_0": "Pacchime bhave sampatte,",
  "tha-ap541:19.2_0": "giribbajapuruttame;",
  "tha-ap541:19.3_0": "Jāto vippakule niddhe,",
  "tha-ap541:19.4_0": "vikalacchādanāsane.",
  "tha-ap541:20.1_0": "Kaṭacchubhikkhaṁ pādāsiṁ,",
  "tha-ap541:20.2_0": "sāriputtassa tādino;",
  "tha-ap541:20.3_0": "Yadā jiṇṇo ca vuddho ca,",
  "tha-ap541:20.4_0": "tadārāmamupāgamiṁ.",
  "tha-ap541:21.1_0": "Pabbajati na maṁ koci,",
  "tha-ap541:21.2_0": "jiṇṇadubbalathāmakaṁ;",
  "tha-ap541:21.3_0": "Tena dīno vivaṇṇaṅgo,",
  "tha-ap541:21.4_0": "soko cāsiṁ tadā ahaṁ.",
  "tha-ap541:22.1_0": "Disvā mahākāruṇiko,",
  "tha-ap541:22.2_0": "mamamāha mahāmuni;",
  "tha-ap541:22.3_0": "‘Kimatthaṁ puttasokaṭṭo,",
  "tha-ap541:22.4_0": "brūhi te cittajaṁ rujaṁ’.",
  "tha-ap541:23.1_0": "‘Pabbajjaṁ na labhe vīra,",
  "tha-ap541:23.2_0": "svākkhāte tava sāsane;",
  "tha-ap541:23.3_0": "Tena sokena dīnosmi,",
  "tha-ap541:23.4_0": "saraṇaṁ hohi nāyaka’.",
  "tha-ap541:24.1_0": "Tadā bhikkhū samānetvā,",
  "tha-ap541:24.2_0": "apucchi munisattamo;",
  "tha-ap541:24.3_0": "‘Imassa adhikāraṁ ye,",
  "tha-ap541:24.4_0": "saranti byāharantu te’.",
  "tha-ap541:25.1_0": "Sāriputto tadāvoca,",
  "tha-ap541:25.2_0": "‘kāramassa sarāmahaṁ;",
  "tha-ap541:25.3_0": "Kaṭacchubhikkhaṁ dāpesi,",
  "tha-ap541:25.4_0": "piṇḍāya carato mama’.",
  "tha-ap541:26.1_0": "‘Sādhu sādhu kataññūsi,",
  "tha-ap541:26.2_0": "sāriputta imaṁ tuvaṁ;",
  "tha-ap541:26.3_0": "Pabbājehi dijaṁ vuḍḍhaṁ,",
  "tha-ap541:26.4_0": "hessatājāniyo ayaṁ’.",
  "tha-ap541:27.1_0": "Tato alatthaṁ pabbajjaṁ,",
  "tha-ap541:27.2_0": "kammavācopasampadaṁ;",
  "tha-ap541:27.3_0": "Na cireneva kālena,",
  "tha-ap541:27.4_0": "pāpuṇiṁ āsavakkhayaṁ.",
  "tha-ap541:28.1_0": "Sakkaccaṁ munino vākyaṁ,",
  "tha-ap541:28.2_0": "suṇāmi mudito yato;",
  "tha-ap541:28.3_0": "Paṭibhāneyyakānaggaṁ,",
  "tha-ap541:28.4_0": "tato maṁ ṭhapayī jino.",
  "tha-ap541:29.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap541:29.2_0": "…pe…",
  "tha-ap541:29.3_0": "viharāmi anāsavo.",
  "tha-ap541:30.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap541:30.2_0": "…pe…",
  "tha-ap541:30.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap541:31.1_0": "Paṭisambhidā catasso,",
  "tha-ap541:31.2_0": "…pe…",
  "tha-ap541:31.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap541:32.1_0": "Itthaṁ sudaṁ āyasmā rādho thero imā gāthāyo abhāsitthāti.",
  "tha-ap541:33.1_0": "Rādhattherassāpadānaṁ navamaṁ."
}