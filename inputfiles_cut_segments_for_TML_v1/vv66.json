{
  "vv66:0.1_0": "Vimānavatthu",
  "vv66:0.2_0": "Purisavimāna",
  "vv66:0.3_0": "Pāyāsivagga",
  "vv66:0.4_0": "2. Dutiyaagāriyavimānavatthu",
  "vv66:1.1_0": "“Yathā vanaṁ cittalataṁ pabhāsati,",
  "vv66:1.2_0": "Uyyānaseṭṭhaṁ tidasānamuttamaṁ;",
  "vv66:1.3_0": "Tathūpamaṁ tuyhamidaṁ vimānaṁ,",
  "vv66:1.4_0": "Obhāsayaṁ tiṭṭhati antalikkhe.",
  "vv66:2.1_0": "Deviddhipattosi mahānubhāvo,",
  "vv66:2.2_0": "Manussabhūto kimakāsi puññaṁ;",
  "vv66:2.3_0": "Kenāsi evaṁ jalitānubhāvo,",
  "vv66:2.4_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv66:3.1_0": "So devaputto attamano,",
  "vv66:3.2_0": "…pe…",
  "vv66:3.3_0": "yassa kammassidaṁ phalaṁ.",
  "vv66:4.1_0": "“Ahañca bhariyā ca manussaloke,",
  "vv66:4.2_0": "Opānabhūtā gharamāvasimha;",
  "vv66:4.3_0": "Annañca pānañca pasannacittā,",
  "vv66:4.4_0": "Sakkacca dānaṁ vipulaṁ adamha.",
  "vv66:5.1_0": "Tena metādiso vaṇṇo,",
  "vv66:5.2_0": "…pe…",
  "vv66:5.3_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv66:6.1_0": "Dutiyaagāriyavimānaṁ dutiyaṁ."
}