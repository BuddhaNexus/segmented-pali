{
  "ja507:0.1_0": "Jātaka",
  "ja507:0.2_0": "Vīsatinipāta",
  "ja507:0.3_0": "Mātaṅgavagga",
  "ja507:0.4_0": "11. Mahāpalobhanajātaka",
  "ja507:1.1_0": "Brahmalokā cavitvāna,",
  "ja507:1.2_0": "devaputto mahiddhiko;",
  "ja507:1.3_0": "Rañño putto udapādi,",
  "ja507:1.4_0": "sabbakāmasamiddhisu.",
  "ja507:2.1_0": "Kāmā vā kāmasaññā vā,",
  "ja507:2.2_0": "brahmaloke na vijjati;",
  "ja507:2.3_0": "Svāssu tāyeva saññāya,",
  "ja507:2.4_0": "kāmehi vijigucchatha.",
  "ja507:3.1_0": "Tassa cantepure āsi,",
  "ja507:3.2_0": "jhānāgāraṁ sumāpitaṁ;",
  "ja507:3.3_0": "So tattha paṭisallīno,",
  "ja507:3.4_0": "eko rahasi jhāyatha.",
  "ja507:4.1_0": "Sa rājā paridevesi,",
  "ja507:4.2_0": "puttasokena aṭṭito;",
  "ja507:4.3_0": "“Ekaputto cayaṁ mayhaṁ,",
  "ja507:4.4_0": "na ca kāmāni bhuñjati.",
  "ja507:5.1_0": "Ko nu khvettha upāyo so,",
  "ja507:5.2_0": "ko vā jānāti kiñcanaṁ;",
  "ja507:5.3_0": "Yo me puttaṁ palobheyya,",
  "ja507:5.4_0": "yathā kāmāni patthaye”.",
  "ja507:6.1_0": "Ahu kumārī tattheva,",
  "ja507:6.2_0": "vaṇṇarūpasamāhitā;",
  "ja507:6.3_0": "Kusalā naccagītassa,",
  "ja507:6.4_0": "vādite ca padakkhiṇā.",
  "ja507:7.1_0": "Sā tattha upasaṅkamma,",
  "ja507:7.2_0": "rājānaṁ etadabravi;",
  "ja507:7.3_0": "“Ahaṁ kho naṁ palobheyyaṁ,",
  "ja507:7.4_0": "sace bhattā bhavissati”.",
  "ja507:8.1_0": "Taṁ tathāvādiniṁ rājā,",
  "ja507:8.2_0": "kumāriṁ etadabravi;",
  "ja507:8.3_0": "“Tvaññeva naṁ palobhehi,",
  "ja507:8.4_0": "tava bhattā bhavissati”.",
  "ja507:9.1_0": "Sā ca antepuraṁ gantvā,",
  "ja507:9.2_0": "bahuṁ kāmupasaṁhitaṁ;",
  "ja507:9.3_0": "Hadayaṅgamā pemanīyā,",
  "ja507:9.4_0": "citrā gāthā abhāsatha.",
  "ja507:10.1_0": "Tassā ca gāyamānāya,",
  "ja507:10.2_0": "saddaṁ sutvāna nāriyā;",
  "ja507:10.3_0": "Kāmacchandassa uppajji,",
  "ja507:10.4_0": "janaṁ so paripucchatha.",
  "ja507:11.1_0": "“Kasseso saddo ko vā so,",
  "ja507:11.2_0": "bhaṇati uccāvacaṁ bahuṁ;",
  "ja507:11.3_0": "Hadayaṅgamaṁ pemanīyaṁ,",
  "ja507:11.4_0": "aho kaṇṇasukhaṁ mama”.",
  "ja507:12.1_0": "“Esā kho pamadā deva,",
  "ja507:12.2_0": "khiḍḍā esā anappikā;",
  "ja507:12.3_0": "Sace tvaṁ kāme bhuñjeyya,",
  "ja507:12.4_0": "bhiyyo bhiyyo chādeyyu taṁ”.",
  "ja507:13.1_0": "“Iṅgha āgacchatorena,",
  "ja507:13.2_0": "avidūramhi gāyatu;",
  "ja507:13.3_0": "Assamassa samīpamhi,",
  "ja507:13.4_0": "santike mayhaṁ gāyatu”.",
  "ja507:14.1_0": "Tirokuṭṭamhi gāyitvā,",
  "ja507:14.2_0": "jhānāgāramhi pāvisi;",
  "ja507:14.3_0": "Bandhi naṁ anupubbena,",
  "ja507:14.4_0": "āraññamiva kuñjaraṁ.",
  "ja507:15.1_0": "Tassa kāmarasaṁ ñatvā,",
  "ja507:15.2_0": "issādhammo ajāyatha;",
  "ja507:15.3_0": "“Ahameva kāme bhuñjeyyaṁ,",
  "ja507:15.4_0": "mā añño puriso ahu”.",
  "ja507:16.1_0": "Tato asiṁ gahetvāna,",
  "ja507:16.2_0": "purise hantuṁ upakkami;",
  "ja507:16.3_0": "“Ahameveko bhuñjissaṁ,",
  "ja507:16.4_0": "mā añño puriso siyā”.",
  "ja507:17.1_0": "Tato jānapadā sabbe,",
  "ja507:17.2_0": "vikkandiṁsu samāgatā;",
  "ja507:17.3_0": "“Putto tyāyaṁ mahārāja,",
  "ja507:17.4_0": "janaṁ heṭhetyadūsakaṁ”.",
  "ja507:18.1_0": "Tañca rājā vivāhesi,",
  "ja507:18.2_0": "samhā raṭṭhā ca khattiyo;",
  "ja507:18.3_0": "“Yāvatā vijitaṁ mayhaṁ,",
  "ja507:18.4_0": "na te vatthabba tāvade”.",
  "ja507:19.1_0": "Tato so bhariyamādāya,",
  "ja507:19.2_0": "samuddaṁ upasaṅkami;",
  "ja507:19.3_0": "Paṇṇasālaṁ karitvāna,",
  "ja507:19.4_0": "vanamuñchāya pāvisi.",
  "ja507:20.1_0": "Athettha isi māgacchi,",
  "ja507:20.2_0": "samuddaṁ uparūpari;",
  "ja507:20.3_0": "So tassa gehaṁ pāvekkhi,",
  "ja507:20.4_0": "bhattakāle upaṭṭhite.",
  "ja507:21.1_0": "Tañca bhariyā palobhesi,",
  "ja507:21.2_0": "passa yāva sudāruṇaṁ;",
  "ja507:21.3_0": "Cuto so brahmacariyamhā,",
  "ja507:21.4_0": "iddhiyā parihāyatha.",
  "ja507:22.1_0": "Rājaputto ca uñchāto,",
  "ja507:22.2_0": "vanamūlaphalaṁ bahuṁ;",
  "ja507:22.3_0": "Sāyaṁ kājena ādāya,",
  "ja507:22.4_0": "assamaṁ upasaṅkami.",
  "ja507:23.1_0": "Isī ca khattiyaṁ disvā,",
  "ja507:23.2_0": "samuddaṁ upasaṅkami;",
  "ja507:23.3_0": "“Vehāyasaṁ gamissan”ti,",
  "ja507:23.4_0": "sīdate so mahaṇṇave.",
  "ja507:24.1_0": "Khattiyo ca isiṁ disvā,",
  "ja507:24.2_0": "sīdamānaṁ mahaṇṇave;",
  "ja507:24.3_0": "Tasseva anukampāya,",
  "ja507:24.4_0": "imā gāthā abhāsatha.",
  "ja507:25.1_0": "“Abhijjamāne vārismiṁ,",
  "ja507:25.2_0": "sayaṁ āgamma iddhiyā;",
  "ja507:25.3_0": "Missībhāvitthiyā gantvā,",
  "ja507:25.4_0": "saṁsīdasi mahaṇṇave.",
  "ja507:26.1_0": "Āvaṭṭanī mahāmāyā,",
  "ja507:26.2_0": "brahmacariyavikopanā;",
  "ja507:26.3_0": "Sīdanti naṁ viditvāna,",
  "ja507:26.4_0": "ārakā parivajjaye.",
  "ja507:27.1_0": "Analā mudusambhāsā,",
  "ja507:27.2_0": "duppūrā tā nadīsamā;",
  "ja507:27.3_0": "Sīdanti naṁ viditvāna,",
  "ja507:27.4_0": "ārakā parivajjaye.",
  "ja507:28.1_0": "Yaṁ etā upasevanti,",
  "ja507:28.2_0": "chandasā vā dhanena vā;",
  "ja507:28.3_0": "Jātavedova saṁ ṭhānaṁ,",
  "ja507:28.4_0": "khippaṁ anudahanti naṁ”.",
  "ja507:29.1_0": "Khattiyassa vaco sutvā,",
  "ja507:29.2_0": "isissa nibbidā ahu;",
  "ja507:29.3_0": "Laddhā porāṇakaṁ maggaṁ,",
  "ja507:29.4_0": "gacchate so vihāyasaṁ.",
  "ja507:30.1_0": "Khattiyo ca isiṁ disvā,",
  "ja507:30.2_0": "gacchamānaṁ vihāyasaṁ;",
  "ja507:30.3_0": "Saṁvegaṁ alabhī dhīro,",
  "ja507:30.4_0": "pabbajjaṁ samarocayi.",
  "ja507:31.1_0": "Tato so pabbajitvāna,",
  "ja507:31.2_0": "kāmarāgaṁ virājayi;",
  "ja507:31.3_0": "Kāmarāgaṁ virājetvā,",
  "ja507:31.4_0": "brahmalokūpago ahūti.",
  "ja507:32.1_0": "Mahāpalobhanajātakaṁ ekādasamaṁ."
}