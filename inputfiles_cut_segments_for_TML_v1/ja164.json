{
  "ja164:0.1_0": "Jātaka",
  "ja164:0.2_0": "Dukanipāta",
  "ja164:0.3_0": "Santhavavagga",
  "ja164:0.4_0": "4. Gijjhajātaka",
  "ja164:1.1_0": "“Yannu gijjho yojanasataṁ,",
  "ja164:1.2_0": "kuṇapāni avekkhati;",
  "ja164:1.3_0": "Kasmā jālañca pāsañca,",
  "ja164:1.4_0": "āsajjāpi na bujjhasi”.",
  "ja164:2.1_0": "“Yadā parābhavo hoti,",
  "ja164:2.2_0": "poso jīvitasaṅkhaye;",
  "ja164:2.3_0": "Atha jālañca pāsañca,",
  "ja164:2.4_0": "āsajjāpi na bujjhatī”ti.",
  "ja164:3.1_0": "Gijjhajātakaṁ catutthaṁ."
}