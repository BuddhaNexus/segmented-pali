{
  "tha-ap181:0.1_0": "Therāpadāna",
  "tha-ap181:0.2_0": "Kumudavagga",
  "tha-ap181:0.3_0": "9. Tikicchakattheraapadāna",
  "tha-ap181:1.1_0": "“Nagare bandhumatiyā,",
  "tha-ap181:1.2_0": "vejjo āsiṁ susikkhito;",
  "tha-ap181:1.3_0": "Āturānaṁ sadukkhānaṁ,",
  "tha-ap181:1.4_0": "mahājanasukhāvaho.",
  "tha-ap181:2.1_0": "Byādhitaṁ samaṇaṁ disvā,",
  "tha-ap181:2.2_0": "sīlavantaṁ mahājutiṁ;",
  "tha-ap181:2.3_0": "Pasannacitto sumano,",
  "tha-ap181:2.4_0": "bhesajjamadadiṁ tadā.",
  "tha-ap181:3.1_0": "Arogo āsi teneva,",
  "tha-ap181:3.2_0": "samaṇo saṁvutindriyo;",
  "tha-ap181:3.3_0": "Asoko nāma nāmena,",
  "tha-ap181:3.4_0": "upaṭṭhāko vipassino.",
  "tha-ap181:4.1_0": "Ekanavutito kappe,",
  "tha-ap181:4.2_0": "yaṁ osadhamadāsahaṁ;",
  "tha-ap181:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap181:4.4_0": "bhesajjassa idaṁ phalaṁ.",
  "tha-ap181:5.1_0": "Ito ca aṭṭhame kappe,",
  "tha-ap181:5.2_0": "sabbosadhasanāmako;",
  "tha-ap181:5.3_0": "Sattaratanasampanno,",
  "tha-ap181:5.4_0": "cakkavattī mahabbalo.",
  "tha-ap181:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap181:6.2_0": "…pe…",
  "tha-ap181:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap181:7.1_0": "Itthaṁ sudaṁ āyasmā tikicchako thero imā gāthāyo abhāsitthāti.",
  "tha-ap181:8.1_0": "Tikicchakattherassāpadānaṁ navamaṁ."
}