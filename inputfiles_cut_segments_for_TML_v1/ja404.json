{
  "ja404:0.1_0": "Jātaka",
  "ja404:0.2_0": "Sattakanipāta",
  "ja404:0.3_0": "Kukkuvagga",
  "ja404:0.4_0": "9. Kapijātaka",
  "ja404:1.1_0": "“Yattha verī nivasati,",
  "ja404:1.2_0": "na vase tattha paṇḍito;",
  "ja404:1.3_0": "Ekarattaṁ dirattaṁ vā,",
  "ja404:1.4_0": "dukkhaṁ vasati verisu.",
  "ja404:2.1_0": "Diso ve lahucittassa,",
  "ja404:2.2_0": "posassānuvidhīyato;",
  "ja404:2.3_0": "Ekassa kapino hetu,",
  "ja404:2.4_0": "yūthassa anayo kato.",
  "ja404:3.1_0": "Bālova paṇḍitamānī,",
  "ja404:3.2_0": "yūthassa parihārako;",
  "ja404:3.3_0": "Sacittassa vasaṁ gantvā,",
  "ja404:3.4_0": "sayethāyaṁ yathā kapi.",
  "ja404:4.1_0": "Na sādhu balavā bālo,",
  "ja404:4.2_0": "yūthassa parihārako;",
  "ja404:4.3_0": "Ahito bhavati ñātīnaṁ,",
  "ja404:4.4_0": "sakuṇānaṁva cetako.",
  "ja404:5.1_0": "Dhīrova balavā sādhu,",
  "ja404:5.2_0": "yūthassa parihārako;",
  "ja404:5.3_0": "Hito bhavati ñātīnaṁ,",
  "ja404:5.4_0": "tidasānaṁva vāsavo.",
  "ja404:6.1_0": "Yo ca sīlañca paññañca,",
  "ja404:6.2_0": "sutañcattani passati;",
  "ja404:6.3_0": "Ubhinnamatthaṁ carati,",
  "ja404:6.4_0": "attano ca parassa ca.",
  "ja404:7.1_0": "Tasmā tuleyya mattānaṁ,",
  "ja404:7.2_0": "sīlapaññāsutāmiva;",
  "ja404:7.3_0": "Gaṇaṁ vā parihare dhīro,",
  "ja404:7.4_0": "eko vāpi paribbaje”ti.",
  "ja404:8.1_0": "Kapijātakaṁ navamaṁ."
}