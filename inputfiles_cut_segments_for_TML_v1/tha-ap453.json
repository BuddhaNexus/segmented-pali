{
  "tha-ap453:0.1_0": "Therāpadāna",
  "tha-ap453:0.2_0": "Jagatidāyakavagga",
  "tha-ap453:0.3_0": "1. Jagatidāyakattheraapadāna",
  "tha-ap453:1.1_0": "“Dhammadassissa munino,",
  "tha-ap453:1.2_0": "bodhiyā pādaputtame;",
  "tha-ap453:1.3_0": "Pasannacitto sumano,",
  "tha-ap453:1.4_0": "jagatiṁ kārayiṁ ahaṁ.",
  "tha-ap453:2.1_0": "Darito pabbatato vā,",
  "tha-ap453:2.2_0": "rukkhato patito ahaṁ;",
  "tha-ap453:2.3_0": "Cuto patiṭṭhaṁ vindāmi,",
  "tha-ap453:2.4_0": "jagatiyā idaṁ phalaṁ.",
  "tha-ap453:3.1_0": "Na me corā vihesanti,",
  "tha-ap453:3.2_0": "nātimaññanti khattiyā;",
  "tha-ap453:3.3_0": "Sabbāmittetikkamāmi,",
  "tha-ap453:3.4_0": "jagatiyā idaṁ phalaṁ.",
  "tha-ap453:4.1_0": "Yaṁ yaṁ yonupapajjāmi,",
  "tha-ap453:4.2_0": "devattaṁ atha mānusaṁ;",
  "tha-ap453:4.3_0": "Sabbattha pūjito homi,",
  "tha-ap453:4.4_0": "jagatiyā idaṁ phalaṁ.",
  "tha-ap453:5.1_0": "Aṭṭhārase kappasate,",
  "tha-ap453:5.2_0": "jagatiṁ kārayiṁ ahaṁ;",
  "tha-ap453:5.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap453:5.4_0": "jagatidānassidaṁ phalaṁ.",
  "tha-ap453:6.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap453:6.2_0": "bhavā sabbe samūhatā;",
  "tha-ap453:6.3_0": "Nāgova bandhanaṁ chetvā,",
  "tha-ap453:6.4_0": "viharāmi anāsavo.",
  "tha-ap453:7.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap453:7.2_0": "mama buddhassa santike;",
  "tha-ap453:7.3_0": "Tisso vijjā anuppattā,",
  "tha-ap453:7.4_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap453:8.1_0": "Paṭisambhidā catasso,",
  "tha-ap453:8.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap453:8.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap453:8.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap453:9.1_0": "Itthaṁ sudaṁ āyasmā jagatidāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap453:10.1_0": "Jagatidāyakattherassāpadānaṁ paṭhamaṁ."
}