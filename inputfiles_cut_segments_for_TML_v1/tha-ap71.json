{
  "tha-ap71:0.1_0": "Therāpadāna",
  "tha-ap71:0.2_0": "Sakacintaniyavagga",
  "tha-ap71:0.3_0": "9. Sumanattheraapadāna",
  "tha-ap71:1.1_0": "“Sumano nāma nāmena,",
  "tha-ap71:1.2_0": "mālākāro ahaṁ tadā;",
  "tha-ap71:1.3_0": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap71:1.4_0": "lokāhutipaṭiggahaṁ.",
  "tha-ap71:2.1_0": "Ubho hatthehi paggayha,",
  "tha-ap71:2.2_0": "sumanaṁ pupphamuttamaṁ;",
  "tha-ap71:2.3_0": "Buddhassa abhiropesiṁ,",
  "tha-ap71:2.4_0": "sikhino lokabandhuno.",
  "tha-ap71:3.1_0": "Imāya pupphapūjāya,",
  "tha-ap71:3.2_0": "cetanāpaṇidhīhi ca;",
  "tha-ap71:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap71:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap71:4.1_0": "Ekattiṁse ito kappe,",
  "tha-ap71:4.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap71:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap71:4.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap71:5.1_0": "Chabbīsatimhi kappamhi,",
  "tha-ap71:5.2_0": "cattārosuṁ mahāyasā;",
  "tha-ap71:5.3_0": "Sattaratanasampannā,",
  "tha-ap71:5.4_0": "rājāno cakkavattino.",
  "tha-ap71:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap71:6.2_0": "…pe…",
  "tha-ap71:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap71:7.1_0": "Itthaṁ sudaṁ āyasmā sumano thero imā gāthāyo abhāsitthāti.",
  "tha-ap71:8.1_0": "Sumanattherassāpadānaṁ navamaṁ."
}