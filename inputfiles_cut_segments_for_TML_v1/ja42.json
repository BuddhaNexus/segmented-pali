{
  "ja42:0.1_0": "Jātaka",
  "ja42:0.2_0": "Ekakanipāta",
  "ja42:0.3_0": "Atthakāmavagga",
  "ja42:0.4_0": "2. Kapotajātaka",
  "ja42:1.1_0": "“Yo atthakāmassa hitānukampino,",
  "ja42:1.2_0": "Ovajjamāno na karoti sāsanaṁ;",
  "ja42:1.3_0": "Kapotakassa vacanaṁ akatvā,",
  "ja42:1.4_0": "Amittahatthatthagatova setī”ti.",
  "ja42:2.1_0": "Kapotajātakaṁ dutiyaṁ."
}