{
  "tha-ap415:0.1_0": "Therāpadāna",
  "tha-ap415:0.2_0": "Bhaddālivagga",
  "tha-ap415:0.3_0": "3 Tiṇasūlakachādaniyattheraapadāna",
  "tha-ap415:1.1_0": "“Jātiṁ jarañca maraṇaṁ,",
  "tha-ap415:1.2_0": "paccavekkhiṁ ahaṁ tadā;",
  "tha-ap415:1.3_0": "Ekako abhinikkhamma,",
  "tha-ap415:1.4_0": "pabbajiṁ anagāriyaṁ.",
  "tha-ap415:2.1_0": "Caramānonupubbena,",
  "tha-ap415:2.2_0": "gaṅgātīraṁ upāgamiṁ;",
  "tha-ap415:2.3_0": "Tatthaddasāsiṁ pathaviṁ,",
  "tha-ap415:2.4_0": "gaṅgātīre samunnataṁ.",
  "tha-ap415:3.1_0": "Assamaṁ tattha māpetvā,",
  "tha-ap415:3.2_0": "vasāmi assame ahaṁ;",
  "tha-ap415:3.3_0": "Sukato caṅkamo mayhaṁ,",
  "tha-ap415:3.4_0": "nānādijagaṇāyuto.",
  "tha-ap415:4.1_0": "Mamupenti ca vissatthā,",
  "tha-ap415:4.2_0": "kūjanti ca manoharaṁ;",
  "tha-ap415:4.3_0": "Ramamāno saha tehi,",
  "tha-ap415:4.4_0": "vasāmi assame ahaṁ.",
  "tha-ap415:5.1_0": "Mama assamasāmantā,",
  "tha-ap415:5.2_0": "migarājā catukkamo;",
  "tha-ap415:5.3_0": "Āsayā abhinikkhamma,",
  "tha-ap415:5.4_0": "gajji so asanī viya.",
  "tha-ap415:6.1_0": "Nadite migarāje ca,",
  "tha-ap415:6.2_0": "hāso me udapajjatha;",
  "tha-ap415:6.3_0": "Migarājaṁ gavesanto,",
  "tha-ap415:6.4_0": "addasaṁ lokanāyakaṁ.",
  "tha-ap415:7.1_0": "Disvānāhaṁ devadevaṁ,",
  "tha-ap415:7.2_0": "tissaṁ lokagganāyakaṁ;",
  "tha-ap415:7.3_0": "Haṭṭho haṭṭhena cittena,",
  "tha-ap415:7.4_0": "pūjayiṁ nāgakesaraṁ.",
  "tha-ap415:8.1_0": "Uggacchantaṁva sūriyaṁ,",
  "tha-ap415:8.2_0": "sālarājaṁva pupphitaṁ;",
  "tha-ap415:8.3_0": "Osadhiṁva virocantaṁ,",
  "tha-ap415:8.4_0": "santhaviṁ lokanāyakaṁ.",
  "tha-ap415:9.1_0": "‘Tava ñāṇena sabbaññu,",
  "tha-ap415:9.2_0": "mocesimaṁ sadevakaṁ;",
  "tha-ap415:9.3_0": "Tavaṁ ārādhayitvāna,",
  "tha-ap415:9.4_0": "jātiyā parimuccare.",
  "tha-ap415:10.1_0": "Adassanena sabbaññu,",
  "tha-ap415:10.2_0": "buddhānaṁ sabbadassinaṁ;",
  "tha-ap415:10.3_0": "Patantivīcinirayaṁ,",
  "tha-ap415:10.4_0": "rāgadosehi ophuṭā.",
  "tha-ap415:11.1_0": "Tava dassanamāgamma,",
  "tha-ap415:11.2_0": "sabbaññu lokanāyaka;",
  "tha-ap415:11.3_0": "Pamuccanti bhavā sabbā,",
  "tha-ap415:11.4_0": "phusanti amataṁ padaṁ.",
  "tha-ap415:12.1_0": "Yadā buddhā cakkhumanto,",
  "tha-ap415:12.2_0": "uppajjanti pabhaṅkarā;",
  "tha-ap415:12.3_0": "Kilese jhāpayitvāna,",
  "tha-ap415:12.4_0": "ālokaṁ dassayanti te’.",
  "tha-ap415:13.1_0": "Kittayitvāna sambuddhaṁ,",
  "tha-ap415:13.2_0": "tissaṁ lokagganāyakaṁ;",
  "tha-ap415:13.3_0": "Haṭṭho haṭṭhena cittena,",
  "tha-ap415:13.4_0": "tiṇasūlaṁ apūjayiṁ.",
  "tha-ap415:14.1_0": "Mama saṅkappamaññāya,",
  "tha-ap415:14.2_0": "tisso lokagganāyako;",
  "tha-ap415:14.3_0": "Sakāsane nisīditvā,",
  "tha-ap415:14.4_0": "imā gāthā abhāsatha.",
  "tha-ap415:15.1_0": "‘Yo maṁ pupphehi chādesi,",
  "tha-ap415:15.2_0": "pasanno sehi pāṇibhi;",
  "tha-ap415:15.3_0": "Tamahaṁ kittayissāmi,",
  "tha-ap415:15.4_0": "suṇātha mama bhāsato.",
  "tha-ap415:16.1_0": "Pañcavīsatikkhattuṁ so,",
  "tha-ap415:16.2_0": "devarajjaṁ karissati;",
  "tha-ap415:16.3_0": "Pañcasattatikkhattuñca,",
  "tha-ap415:16.4_0": "cakkavattī bhavissati.",
  "tha-ap415:17.1_0": "Padesarajjaṁ vipulaṁ,",
  "tha-ap415:17.2_0": "gaṇanāto asaṅkhiyaṁ;",
  "tha-ap415:17.3_0": "Tassa kammanissandena,",
  "tha-ap415:17.4_0": "pupphānaṁ pūjanāya ca.",
  "tha-ap415:18.1_0": "Sīsaṁnhāto cayaṁ poso,",
  "tha-ap415:18.2_0": "pupphamākaṅkhate yadi;",
  "tha-ap415:18.3_0": "Puññakammena saṁyuttaṁ,",
  "tha-ap415:18.4_0": "purato pātubhavissati.",
  "tha-ap415:19.1_0": "Yaṁ yaṁ icchati kāmehi,",
  "tha-ap415:19.2_0": "taṁ taṁ pātubhavissati;",
  "tha-ap415:19.3_0": "Saṅkappaṁ paripūretvā,",
  "tha-ap415:19.4_0": "nibbāyissatināsavo’.",
  "tha-ap415:20.1_0": "Aṭṭhārasamaṁ bhāṇavāraṁ.",
  "tha-ap415:21.1_0": "Kilese jhāpayitvāna,",
  "tha-ap415:21.2_0": "sampajāno patissato;",
  "tha-ap415:21.3_0": "Ekāsane nisīditvā,",
  "tha-ap415:21.4_0": "arahattamapāpuṇiṁ.",
  "tha-ap415:22.1_0": "Caṅkamanto nipajjanto,",
  "tha-ap415:22.2_0": "nisinno uda vā ṭhito;",
  "tha-ap415:22.3_0": "Buddhaseṭṭhaṁ saritvāna,",
  "tha-ap415:22.4_0": "viharāmi ahaṁ sadā.",
  "tha-ap415:23.1_0": "Cīvare piṇḍapāte ca,",
  "tha-ap415:23.2_0": "paccaye sayanāsane;",
  "tha-ap415:23.3_0": "Tattha me ūnatā natthi,",
  "tha-ap415:23.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap415:24.1_0": "So dāni patto amataṁ,",
  "tha-ap415:24.2_0": "santaṁ padamanuttaraṁ;",
  "tha-ap415:24.3_0": "Sabbāsave pariññāya,",
  "tha-ap415:24.4_0": "viharāmi anāsavo.",
  "tha-ap415:25.1_0": "Dvenavute ito kappe,",
  "tha-ap415:25.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap415:25.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap415:25.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap415:26.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap415:26.2_0": "…pe…",
  "tha-ap415:26.3_0": "viharāmi anāsavo.",
  "tha-ap415:27.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap415:27.2_0": "…pe…",
  "tha-ap415:27.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap415:28.1_0": "Paṭisambhidā catasso,",
  "tha-ap415:28.2_0": "…pe…",
  "tha-ap415:28.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap415:29.1_0": "Itthaṁ sudaṁ āyasmā tiṇasūlakachādaniyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap415:30.1_0": "Tiṇasūlakachādaniyattherassāpadānaṁ tatiyaṁ."
}