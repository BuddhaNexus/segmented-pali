{
  "vv20:0.1_0": "Vimānavatthu",
  "vv20:0.2_0": "Itthivimāna",
  "vv20:0.3_0": "Cittalatāvagga",
  "vv20:0.4_0": "3. Ācāmadāyikāvimānavatthu",
  "vv20:1.1_0": "“Piṇḍāya te carantassa,",
  "vv20:1.2_0": "tuṇhībhūtassa tiṭṭhato;",
  "vv20:1.3_0": "Daliddā kapaṇā nārī,",
  "vv20:1.4_0": "parāgāraṁ apassitā.",
  "vv20:2.1_0": "Yā te adāsi ācāmaṁ,",
  "vv20:2.2_0": "pasannā sehi pāṇibhi;",
  "vv20:2.3_0": "Sā hitvā mānusaṁ dehaṁ,",
  "vv20:2.4_0": "kaṁ nu sā disataṁ gatā”ti.",
  "vv20:3.1_0": "“Piṇḍāya me carantassa,",
  "vv20:3.2_0": "tuṇhībhūtassa tiṭṭhato;",
  "vv20:3.3_0": "Daliddā kapaṇā nārī,",
  "vv20:3.4_0": "parāgāraṁ apassitā.",
  "vv20:4.1_0": "Yā me adāsi ācāmaṁ,",
  "vv20:4.2_0": "pasannā sehi pāṇibhi;",
  "vv20:4.3_0": "Sā hitvā mānusaṁ dehaṁ,",
  "vv20:4.4_0": "vippamuttā ito cutā.",
  "vv20:5.1_0": "Nimmānaratino nāma,",
  "vv20:5.2_0": "santi devā mahiddhikā;",
  "vv20:5.3_0": "Tattha sā sukhitā nārī,",
  "vv20:5.4_0": "modatācāmadāyikā”ti.",
  "vv20:6.1_0": "“Aho dānaṁ varākiyā,",
  "vv20:6.2_0": "kassape suppatiṭṭhitaṁ;",
  "vv20:6.3_0": "Parābhatena dānena,",
  "vv20:6.4_0": "ijjhittha vata dakkhiṇā.",
  "vv20:7.1_0": "Yā mahesittaṁ kāreyya,",
  "vv20:7.2_0": "cakkavattissa rājino;",
  "vv20:7.3_0": "Nārī sabbaṅgakalyāṇī,",
  "vv20:7.4_0": "bhattu cānomadassikā;",
  "vv20:7.5_0": "Etassācāmadānassa,",
  "vv20:7.6_0": "kalaṁ nāgghati soḷasiṁ.",
  "vv20:8.1_0": "Sataṁ nikkhā sataṁ assā,",
  "vv20:8.2_0": "sataṁ assatarīrathā;",
  "vv20:8.3_0": "Sataṁ kaññāsahassāni,",
  "vv20:8.4_0": "āmuttamaṇikuṇḍalā;",
  "vv20:8.5_0": "Etassācāmadānassa,",
  "vv20:8.6_0": "kalaṁ nāgghanti soḷasiṁ.",
  "vv20:9.1_0": "Sataṁ hemavatā nāgā,",
  "vv20:9.2_0": "īsādantā urūḷhavā;",
  "vv20:9.3_0": "Suvaṇṇakacchā mātaṅgā,",
  "vv20:9.4_0": "hemakappanavāsasā;",
  "vv20:9.5_0": "Etassācāmadānassa,",
  "vv20:9.6_0": "kalaṁ nāgghati soḷasiṁ.",
  "vv20:10.1_0": "Catunnamapi dīpānaṁ,",
  "vv20:10.2_0": "issaraṁ yodha kāraye;",
  "vv20:10.3_0": "Etassācāmadānassa,",
  "vv20:10.4_0": "kalaṁ nāgghati soḷasin”ti.",
  "vv20:11.1_0": "Ācāmadāyikāvimānaṁ tatiyaṁ."
}