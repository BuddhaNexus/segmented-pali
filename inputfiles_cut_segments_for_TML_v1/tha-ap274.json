{
  "tha-ap274:0.1_0": "Therāpadāna",
  "tha-ap274:0.2_0": "Suvaṇṇabibbohanavagga",
  "tha-ap274:0.3_0": "2 Tilamuṭṭhidāyakattheraapadāna",
  "tha-ap274:1.1_0": "“Mama saṅkappamaññāya,",
  "tha-ap274:1.2_0": "satthā lokagganāyako;",
  "tha-ap274:1.3_0": "Manomayena kāyena,",
  "tha-ap274:1.4_0": "iddhiyā upasaṅkami.",
  "tha-ap274:2.1_0": "Satthāraṁ upasaṅkantaṁ,",
  "tha-ap274:2.2_0": "vanditvā purisuttamaṁ;",
  "tha-ap274:2.3_0": "Pasannacitto sumano,",
  "tha-ap274:2.4_0": "tilamuṭṭhimadāsahaṁ.",
  "tha-ap274:3.1_0": "Ekanavutito kappe,",
  "tha-ap274:3.2_0": "yaṁ dānamadadiṁ tadā;",
  "tha-ap274:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap274:3.4_0": "tilamuṭṭhiyidaṁ phalaṁ.",
  "tha-ap274:4.1_0": "Ito soḷasakappamhi,",
  "tha-ap274:4.2_0": "tantiso nāma khattiyo;",
  "tha-ap274:4.3_0": "Sattaratanasampanno,",
  "tha-ap274:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap274:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap274:5.2_0": "…pe…",
  "tha-ap274:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap274:6.1_0": "Itthaṁ sudaṁ āyasmā tilamuṭṭhidāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap274:7.1_0": "Tilamuṭṭhidāyakattherassāpadānaṁ dutiyaṁ."
}