{
  "ja156:0.1_0": "Jātaka",
  "ja156:0.2_0": "Dukanipāta",
  "ja156:0.3_0": "Daḷhavagga",
  "ja156:0.4_0": "6. Alīnacittajātaka",
  "ja156:1.1_0": "“Alīnacittaṁ nissāya,",
  "ja156:1.2_0": "pahaṭṭhā mahatī camū;",
  "ja156:1.3_0": "Kosalaṁ senāsantuṭṭhaṁ,",
  "ja156:1.4_0": "jīvaggāhaṁ agāhayi.",
  "ja156:2.1_0": "Evaṁ nissayasampanno,",
  "ja156:2.2_0": "bhikkhu āraddhavīriyo;",
  "ja156:2.3_0": "Bhāvayaṁ kusalaṁ dhammaṁ,",
  "ja156:2.4_0": "yogakkhemassa pattiyā;",
  "ja156:2.5_0": "Pāpuṇe anupubbena,",
  "ja156:2.6_0": "sabbasaṁyojanakkhayan”ti.",
  "ja156:3.1_0": "Alīnacittajātakaṁ chaṭṭhaṁ."
}