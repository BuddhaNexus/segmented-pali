{
  "ja342:0.1_0": "Jātaka",
  "ja342:0.2_0": "Catukkanipāta",
  "ja342:0.3_0": "Cūḷakuṇālavagga",
  "ja342:0.4_0": "2. Vānarajātaka",
  "ja342:1.1_0": "“Asakkhiṁ vata attānaṁ,",
  "ja342:1.2_0": "uddhātuṁ udakā thalaṁ;",
  "ja342:1.3_0": "Na dānāhaṁ puna tuyhaṁ,",
  "ja342:1.4_0": "vasaṁ gacchāmi vārija.",
  "ja342:2.1_0": "Alametehi ambehi,",
  "ja342:2.2_0": "jambūhi panasehi ca;",
  "ja342:2.3_0": "Yāni pāraṁ samuddassa,",
  "ja342:2.4_0": "varaṁ mayhaṁ udumbaro.",
  "ja342:3.1_0": "Yo ca uppatitaṁ atthaṁ,",
  "ja342:3.2_0": "na khippamanubujjhati;",
  "ja342:3.3_0": "Amittavasamanveti,",
  "ja342:3.4_0": "pacchā ca anutappati.",
  "ja342:4.1_0": "Yo ca uppatitaṁ atthaṁ,",
  "ja342:4.2_0": "khippameva nibodhati;",
  "ja342:4.3_0": "Muccate sattusambādhā,",
  "ja342:4.4_0": "na ca pacchānutappatī”ti.",
  "ja342:5.1_0": "Vānarajātakaṁ dutiyaṁ."
}