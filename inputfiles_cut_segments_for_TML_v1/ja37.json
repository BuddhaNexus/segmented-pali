{
  "ja37:0.1_0": "Jātaka",
  "ja37:0.2_0": "Ekakanipāta",
  "ja37:0.3_0": "Kulāvakavagga",
  "ja37:0.4_0": "7. Tittirajātaka",
  "ja37:1.1_0": "“Ye vuḍḍhamapacāyanti,",
  "ja37:1.2_0": "narā dhammassa kovidā;",
  "ja37:1.3_0": "Diṭṭheva dhamme pāsaṁsā,",
  "ja37:1.4_0": "samparāye ca suggatī”ti.",
  "ja37:2.1_0": "Tittirajātakaṁ sattamaṁ."
}