{
  "ja365:0.1_0": "Jātaka",
  "ja365:0.2_0": "Pañcakanipāta",
  "ja365:0.3_0": "Vaṇṇārohavagga",
  "ja365:0.4_0": "5. Ahituṇḍikajātaka",
  "ja365:1.1_0": "“Dhuttomhi samma sumukha,",
  "ja365:1.2_0": "jūte akkhaparājito;",
  "ja365:1.3_0": "Harehi ambapakkāni,",
  "ja365:1.4_0": "vīriyaṁ te bhakkhayāmase”.",
  "ja365:2.1_0": "“Alikaṁ vata maṁ samma,",
  "ja365:2.2_0": "abhūtena pasaṁsasi;",
  "ja365:2.3_0": "Ko te suto vā diṭṭho vā,",
  "ja365:2.4_0": "sumukho nāma makkaṭo.",
  "ja365:3.1_0": "Ajjāpi me taṁ manasi,",
  "ja365:3.2_0": "yaṁ maṁ tvaṁ ahituṇḍika;",
  "ja365:3.3_0": "Dhaññāpaṇaṁ pavisitvā,",
  "ja365:3.4_0": "matto chātaṁ hanāsi maṁ.",
  "ja365:4.1_0": "Tāhaṁ saraṁ dukkhaseyyaṁ,",
  "ja365:4.2_0": "api rajjampi kāraye;",
  "ja365:4.3_0": "Nevāhaṁ yācito dajjaṁ,",
  "ja365:4.4_0": "tathā hi bhayatajjito.",
  "ja365:5.1_0": "Yañca jaññā kule jātaṁ,",
  "ja365:5.2_0": "Gabbhe tittaṁ amacchariṁ;",
  "ja365:5.3_0": "Tena sakhiñca mittañca,",
  "ja365:5.4_0": "Dhīro sandhātumarahatī”ti.",
  "ja365:6.1_0": "Ahituṇḍikajātakaṁ pañcamaṁ."
}