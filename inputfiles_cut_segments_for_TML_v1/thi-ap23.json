{
  "thi-ap23:0.1_0": "Therīapadāna",
  "thi-ap23:0.2_0": "Kuṇḍalakesīvagga",
  "thi-ap23:0.3_0": "3. Dhammadinnātherīapadāna",
  "thi-ap23:1.1_0": "“Padumuttaro nāma jino,",
  "thi-ap23:1.2_0": "sabbadhammāna pāragū;",
  "thi-ap23:1.3_0": "Ito satasahassamhi,",
  "thi-ap23:1.4_0": "kappe uppajji nāyako.",
  "thi-ap23:2.1_0": "Tadāhaṁ haṁsavatiyaṁ,",
  "thi-ap23:2.2_0": "kule aññatare ahuṁ;",
  "thi-ap23:2.3_0": "Parakammakārī āsiṁ,",
  "thi-ap23:2.4_0": "nipakā sīlasaṁvutā.",
  "thi-ap23:3.1_0": "Padumuttarabuddhassa,",
  "thi-ap23:3.2_0": "sujāto aggasāvako;",
  "thi-ap23:3.3_0": "Vihārā abhinikkhamma,",
  "thi-ap23:3.4_0": "piṇḍapātāya gacchati.",
  "thi-ap23:4.1_0": "Ghaṭaṁ gahetvā gacchantī,",
  "thi-ap23:4.2_0": "tadā udakahārikā;",
  "thi-ap23:4.3_0": "Taṁ disvā adadaṁ pūpaṁ,",
  "thi-ap23:4.4_0": "pasannā sehi pāṇibhi.",
  "thi-ap23:5.1_0": "Paṭiggahetvā tattheva,",
  "thi-ap23:5.2_0": "nisinno paribhuñji so;",
  "thi-ap23:5.3_0": "Tato netvāna taṁ gehaṁ,",
  "thi-ap23:5.4_0": "adāsiṁ tassa bhojanaṁ.",
  "thi-ap23:6.1_0": "Tato me ayyako tuṭṭho,",
  "thi-ap23:6.2_0": "akarī suṇisaṁ sakaṁ;",
  "thi-ap23:6.3_0": "Sassuyā saha gantvāna,",
  "thi-ap23:6.4_0": "sambuddhaṁ abhivādayiṁ.",
  "thi-ap23:7.1_0": "Tadā so dhammakathikaṁ,",
  "thi-ap23:7.2_0": "bhikkhuniṁ parikittayaṁ;",
  "thi-ap23:7.3_0": "Ṭhapesi etadaggamhi,",
  "thi-ap23:7.4_0": "taṁ sutvā muditā ahaṁ.",
  "thi-ap23:8.1_0": "Nimantayitvā sugataṁ,",
  "thi-ap23:8.2_0": "sasaṅghaṁ lokanāyakaṁ;",
  "thi-ap23:8.3_0": "Mahādānaṁ daditvāna,",
  "thi-ap23:8.4_0": "taṁ ṭhānamabhipatthayiṁ.",
  "thi-ap23:9.1_0": "Tato maṁ sugato āha,",
  "thi-ap23:9.2_0": "ghananinnādasussaro;",
  "thi-ap23:9.3_0": "‘Mamupaṭṭhānanirate,",
  "thi-ap23:9.4_0": "sasaṅghaparivesike.",
  "thi-ap23:10.1_0": "Saddhammassavane yutte,",
  "thi-ap23:10.2_0": "guṇavaddhitamānase;",
  "thi-ap23:10.3_0": "Bhadde bhavassu muditā,",
  "thi-ap23:10.4_0": "lacchase paṇidhīphalaṁ.",
  "thi-ap23:11.1_0": "Satasahassito kappe,",
  "thi-ap23:11.2_0": "okkākakulasambhavo;",
  "thi-ap23:11.3_0": "Gotamo nāma gottena,",
  "thi-ap23:11.4_0": "satthā loke bhavissati.",
  "thi-ap23:12.1_0": "Tassa dhammesu dāyādā,",
  "thi-ap23:12.2_0": "orasā dhammanimmitā;",
  "thi-ap23:12.3_0": "Dhammadinnāti nāmena,",
  "thi-ap23:12.4_0": "hessati satthu sāvikā’.",
  "thi-ap23:13.1_0": "Taṁ sutvā muditā hutvā,",
  "thi-ap23:13.2_0": "yāvajīvaṁ mahāmuniṁ;",
  "thi-ap23:13.3_0": "Mettacittā paricariṁ,",
  "thi-ap23:13.4_0": "paccayehi vināyakaṁ.",
  "thi-ap23:14.1_0": "Tena kammena sukatena,",
  "thi-ap23:14.2_0": "cetanāpaṇidhīhi ca;",
  "thi-ap23:14.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "thi-ap23:14.4_0": "tāvatiṁsamagacchahaṁ.",
  "thi-ap23:15.1_0": "Imamhi bhaddake kappe,",
  "thi-ap23:15.2_0": "brahmabandhu mahāyaso;",
  "thi-ap23:15.3_0": "Kassapo nāma gottena,",
  "thi-ap23:15.4_0": "uppajji vadataṁ varo.",
  "thi-ap23:16.1_0": "Upaṭṭhāko mahesissa,",
  "thi-ap23:16.2_0": "tadā āsi narissaro;",
  "thi-ap23:16.3_0": "Kāsirājā kikī nāma,",
  "thi-ap23:16.4_0": "bārāṇasipuruttame.",
  "thi-ap23:17.1_0": "Chaṭṭhā tassāsahaṁ dhītā,",
  "thi-ap23:17.2_0": "sudhammā iti vissutā;",
  "thi-ap23:17.3_0": "Dhammaṁ sutvā jinaggassa,",
  "thi-ap23:17.4_0": "pabbajjaṁ samarocayiṁ.",
  "thi-ap23:18.1_0": "Anujāni na no tāto,",
  "thi-ap23:18.2_0": "agāreva tadā mayaṁ;",
  "thi-ap23:18.3_0": "Vīsavassasahassāni,",
  "thi-ap23:18.4_0": "vicarimha atanditā.",
  "thi-ap23:19.1_0": "Tatiyaṁ bhāṇavāraṁ.",
  "thi-ap23:20.1_0": "Komāribrahmacariyaṁ,",
  "thi-ap23:20.2_0": "rājakaññā sukhedhitā;",
  "thi-ap23:20.3_0": "Buddhopaṭṭhānaniratā,",
  "thi-ap23:20.4_0": "muditā satta dhītaro.",
  "thi-ap23:21.1_0": "Samaṇī samaṇaguttā ca,",
  "thi-ap23:21.2_0": "bhikkhunī bhikkhudāyikā;",
  "thi-ap23:21.3_0": "Dhammā ceva sudhammā ca,",
  "thi-ap23:21.4_0": "sattamī saṅghadāyikā.",
  "thi-ap23:22.1_0": "Khemā uppalavaṇṇā ca,",
  "thi-ap23:22.2_0": "paṭācārā ca kuṇḍalā;",
  "thi-ap23:22.3_0": "Gotamī ca ahañceva,",
  "thi-ap23:22.4_0": "visākhā hoti sattamī.",
  "thi-ap23:23.1_0": "Tehi kammehi sukatehi,",
  "thi-ap23:23.2_0": "cetanāpaṇidhīhi ca;",
  "thi-ap23:23.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "thi-ap23:23.4_0": "tāvatiṁsamagacchahaṁ.",
  "thi-ap23:24.1_0": "Pacchime ca bhave dāni,",
  "thi-ap23:24.2_0": "giribbajapuruttame;",
  "thi-ap23:24.3_0": "Jātā seṭṭhikule phīte,",
  "thi-ap23:24.4_0": "sabbakāmasamiddhine.",
  "thi-ap23:25.1_0": "Yadā rūpaguṇūpetā,",
  "thi-ap23:25.2_0": "paṭhame yobbane ṭhitā;",
  "thi-ap23:25.3_0": "Tadā parakulaṁ gantvā,",
  "thi-ap23:25.4_0": "vasiṁ sukhasamappitā.",
  "thi-ap23:26.1_0": "Upetvā lokasaraṇaṁ,",
  "thi-ap23:26.2_0": "suṇitvā dhammadesanaṁ;",
  "thi-ap23:26.3_0": "Anāgāmiphalaṁ patto,",
  "thi-ap23:26.4_0": "sāmiko me subuddhimā.",
  "thi-ap23:27.1_0": "Tadāhaṁ anujānetvā,",
  "thi-ap23:27.2_0": "pabbajiṁ anagāriyaṁ;",
  "thi-ap23:27.3_0": "Na cireneva kālena,",
  "thi-ap23:27.4_0": "arahattamapāpuṇiṁ.",
  "thi-ap23:28.1_0": "Tadā upāsako so maṁ,",
  "thi-ap23:28.2_0": "upagantvā apucchatha;",
  "thi-ap23:28.3_0": "Gambhīre nipuṇe pañhe,",
  "thi-ap23:28.4_0": "te sabbe byākariṁ ahaṁ.",
  "thi-ap23:29.1_0": "Jino tasmiṁ guṇe tuṭṭho,",
  "thi-ap23:29.2_0": "etadagge ṭhapesi maṁ;",
  "thi-ap23:29.3_0": "‘Bhikkhuniṁ dhammakathikaṁ,",
  "thi-ap23:29.4_0": "nāññaṁ passāmi edisiṁ.",
  "thi-ap23:30.1_0": "Dhammadinnā yathā dhīrā,",
  "thi-ap23:30.2_0": "evaṁ dhāretha bhikkhavo’;",
  "thi-ap23:30.3_0": "Evāhaṁ paṇḍitā homi,",
  "thi-ap23:30.4_0": "nāyakenānukampitā.",
  "thi-ap23:31.1_0": "Pariciṇṇo mayā satthā,",
  "thi-ap23:31.2_0": "kataṁ buddhassa sāsanaṁ;",
  "thi-ap23:31.3_0": "Ohito garuko bhāro,",
  "thi-ap23:31.4_0": "bhavanetti samūhatā.",
  "thi-ap23:32.1_0": "Yassatthāya pabbajitā,",
  "thi-ap23:32.2_0": "agārasmānagāriyaṁ;",
  "thi-ap23:32.3_0": "So me attho anuppatto,",
  "thi-ap23:32.4_0": "sabbasaṁyojanakkhayo.",
  "thi-ap23:33.1_0": "Iddhīsu ca vasī homi,",
  "thi-ap23:33.2_0": "dibbāya sotadhātuyā;",
  "thi-ap23:33.3_0": "Paracittāni jānāmi,",
  "thi-ap23:33.4_0": "satthusāsanakārikā.",
  "thi-ap23:34.1_0": "Pubbenivāsaṁ jānāmi,",
  "thi-ap23:34.2_0": "dibbacakkhu visodhitaṁ;",
  "thi-ap23:34.3_0": "Khepetvā āsave sabbe,",
  "thi-ap23:34.4_0": "visuddhāsiṁ sunimmalā.",
  "thi-ap23:35.1_0": "Kilesā jhāpitā mayhaṁ,",
  "thi-ap23:35.2_0": "…pe…",
  "thi-ap23:35.3_0": "viharāmi anāsavā.",
  "thi-ap23:36.1_0": "Svāgataṁ vata me āsi,",
  "thi-ap23:36.2_0": "…pe…",
  "thi-ap23:36.3_0": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap23:37.1_0": "Paṭisambhidā catasso,",
  "thi-ap23:37.2_0": "…pe…",
  "thi-ap23:37.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "thi-ap23:38.1_0": "Itthaṁ sudaṁ dhammadinnā bhikkhunī imā gāthāyo abhāsitthāti.",
  "thi-ap23:39.1_0": "Dhammadinnātheriyāpadānaṁ tatiyaṁ."
}