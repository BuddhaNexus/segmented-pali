{
  "thag1.49:0.1_0": "Theragāthā",
  "thag1.49:0.2_0": "Ekakanipāta",
  "thag1.49:0.3_0": "Pañcamavagga",
  "thag1.49:0.4_0": "1.49. Rāmaṇeyyakattheragāthā",
  "thag1.49:1.1_0": "“Cihacihābhinadite,",
  "thag1.49:1.2_0": "sippikābhirutehi ca;",
  "thag1.49:1.3_0": "Na me taṁ phandati cittaṁ,",
  "thag1.49:1.4_0": "ekattanirataṁ hi me”ti.",
  "thag1.49:1.5_0": "… Rāmaṇeyyako thero …."
}