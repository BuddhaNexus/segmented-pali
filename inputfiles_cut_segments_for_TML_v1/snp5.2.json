{
  "snp5.2:0.1_0": "Sutta Nipāta 5.2",
  "snp5.2:0.2_0": "Ajitamāṇavapucchā",
  "snp5.2:1.1_0": "“Kenassu nivuto loko,",
  "snp5.2:1.2_0": "(iccāyasmā ajito)",
  "snp5.2:1.3_0": "Kenassu nappakāsati;",
  "snp5.2:1.4_0": "Kissābhilepanaṁ brūsi,",
  "snp5.2:1.5_0": "Kiṁsu tassa mahabbhayaṁ”.",
  "snp5.2:2.1_0": "“Avijjāya nivuto loko,",
  "snp5.2:2.2_0": "(ajitāti bhagavā)",
  "snp5.2:2.3_0": "Vevicchā pamādā nappakāsati;",
  "snp5.2:2.4_0": "Jappābhilepanaṁ brūmi,",
  "snp5.2:2.5_0": "Dukkhamassa mahabbhayaṁ”.",
  "snp5.2:3.1_0": "“Savanti sabbadhi sotā,",
  "snp5.2:3.2_0": "(iccāyasmā ajito)",
  "snp5.2:3.3_0": "Sotānaṁ kiṁ nivāraṇaṁ;",
  "snp5.2:3.4_0": "Sotānaṁ saṁvaraṁ brūhi,",
  "snp5.2:3.5_0": "Kena sotā pidhiyyare”.",
  "snp5.2:4.1_0": "“Yāni sotāni lokasmiṁ,",
  "snp5.2:4.2_0": "(ajitāti bhagavā)",
  "snp5.2:4.3_0": "Sati tesaṁ nivāraṇaṁ;",
  "snp5.2:4.4_0": "Sotānaṁ saṁvaraṁ brūmi,",
  "snp5.2:4.5_0": "Paññāyete pidhiyyare”.",
  "snp5.2:5.1_0": "“Paññā ceva sati yañca,",
  "snp5.2:5.2_0": "(iccāyasmā ajito)",
  "snp5.2:5.3_0": "Nāmarūpañca mārisa;",
  "snp5.2:5.4_0": "Etaṁ me puṭṭho pabrūhi,",
  "snp5.2:5.5_0": "Katthetaṁ uparujjhati”.",
  "snp5.2:6.1_0": "“Yametaṁ pañhaṁ apucchi,",
  "snp5.2:6.2_0": "ajita taṁ vadāmi te;",
  "snp5.2:6.3_0": "Yattha nāmañca rūpañca,",
  "snp5.2:6.4_0": "asesaṁ uparujjhati;",
  "snp5.2:6.5_0": "Viññāṇassa nirodhena,",
  "snp5.2:6.6_0": "etthetaṁ uparujjhati”.",
  "snp5.2:7.1_0": "“Ye ca saṅkhātadhammāse,",
  "snp5.2:7.2_0": "ye ca sekhā puthū idha;",
  "snp5.2:7.3_0": "Tesaṁ me nipako iriyaṁ,",
  "snp5.2:7.4_0": "puṭṭho pabrūhi mārisa”.",
  "snp5.2:8.1_0": "“Kāmesu nābhigijjheyya,",
  "snp5.2:8.2_0": "Manasānāvilo siyā;",
  "snp5.2:8.3_0": "Kusalo sabbadhammānaṁ,",
  "snp5.2:8.4_0": "Sato bhikkhu paribbaje”ti.",
  "snp5.2:8.5_0": "Ajitamāṇavapucchā paṭhamā."
}