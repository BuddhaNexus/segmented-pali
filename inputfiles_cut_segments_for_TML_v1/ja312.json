{
  "ja312:0.1_0": "Jātaka",
  "ja312:0.2_0": "Catukkanipāta",
  "ja312:0.3_0": "Pucimandavagga",
  "ja312:0.4_0": "2. Kassapamandiyajātaka",
  "ja312:1.1_0": "“Api kassapa mandiyā,",
  "ja312:1.2_0": "yuvā sapati hanti vā;",
  "ja312:1.3_0": "Sabbaṁ taṁ khamate dhīro,",
  "ja312:1.4_0": "paṇḍito taṁ titikkhati.",
  "ja312:2.1_0": "Sacepi santo vivadanti,",
  "ja312:2.2_0": "khippaṁ sandhīyare puna;",
  "ja312:2.3_0": "Bālā pattāva bhijjanti,",
  "ja312:2.4_0": "na te samathamajjhagū.",
  "ja312:3.1_0": "Ete bhiyyo samāyanti,",
  "ja312:3.2_0": "sandhi tesaṁ na jīrati;",
  "ja312:3.3_0": "Yo cādhipannaṁ jānāti,",
  "ja312:3.4_0": "yo ca jānāti desanaṁ.",
  "ja312:4.1_0": "Eso hi uttaritaro,",
  "ja312:4.2_0": "Bhāravaho dhuraddharo;",
  "ja312:4.3_0": "Yo paresādhipannānaṁ,",
  "ja312:4.4_0": "Sayaṁ sandhātumarahatī”ti.",
  "ja312:5.1_0": "Kassapamandiyajātakaṁ dutiyaṁ."
}