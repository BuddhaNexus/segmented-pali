{
  "thig7.3:0.1_0": "Therīgāthā",
  "thig7.3:0.2_0": "Sattakanipāta",
  "thig7.3:0.3_0": "3. Upacālātherīgāthā",
  "thig7.3:1.1_0": "“Satimatī cakkhumatī,",
  "thig7.3:1.2_0": "bhikkhunī bhāvitindriyā;",
  "thig7.3:1.3_0": "Paṭivijjhi padaṁ santaṁ,",
  "thig7.3:1.4_0": "akāpurisasevitaṁ”.",
  "thig7.3:2.1_0": "“Kiṁ nu jātiṁ na rocesi,",
  "thig7.3:2.2_0": "jāto kāmāni bhuñjati;",
  "thig7.3:2.3_0": "Bhuñjāhi kāmaratiyo,",
  "thig7.3:2.4_0": "māhu pacchānutāpinī”.",
  "thig7.3:3.1_0": "“Jātassa maraṇaṁ hoti,",
  "thig7.3:3.2_0": "hatthapādāna chedanaṁ;",
  "thig7.3:3.3_0": "Vadhabandhapariklesaṁ,",
  "thig7.3:3.4_0": "jāto dukkhaṁ nigacchati.",
  "thig7.3:4.1_0": "Atthi sakyakule jāto,",
  "thig7.3:4.2_0": "sambuddho aparājito;",
  "thig7.3:4.3_0": "So me dhammamadesesi,",
  "thig7.3:4.4_0": "jātiyā samatikkamaṁ.",
  "thig7.3:5.1_0": "Dukkhaṁ dukkhasamuppādaṁ,",
  "thig7.3:5.2_0": "dukkhassa ca atikkamaṁ;",
  "thig7.3:5.3_0": "Ariyaṁ caṭṭhaṅgikaṁ maggaṁ,",
  "thig7.3:5.4_0": "dukkhūpasamagāminaṁ.",
  "thig7.3:6.1_0": "Tassāhaṁ vacanaṁ sutvā,",
  "thig7.3:6.2_0": "vihariṁ sāsane ratā;",
  "thig7.3:6.3_0": "Tisso vijjā anuppattā,",
  "thig7.3:6.4_0": "kataṁ buddhassa sāsanaṁ.",
  "thig7.3:7.1_0": "Sabbattha vihatā nandī,",
  "thig7.3:7.2_0": "tamokhandho padālito;",
  "thig7.3:7.3_0": "Evaṁ jānāhi pāpima,",
  "thig7.3:7.4_0": "nihato tvamasi antaka”.",
  "thig7.3:8.1_0": "…",
  "thig7.3:8.2_0": "Upacālā therī ….",
  "thig7.3:9.1_0": "Sattakanipāto niṭṭhito."
}