{
  "kv3.7:0.1_0": "Kathāvatthu",
  "kv3.7:0.2_0": "Mahāpaṇṇāsaka",
  "kv3.7:0.3_0": "Tatiyavagga",
  "kv3.7:0.4_0": "Dibbacakkhukathā",
  "kv3.7:1.1_0": "Maṁsacakkhuṁ dhammupatthaddhaṁ dibbacakkhuṁ hotīti?",
  "kv3.7:1.2_0": "Āmantā.",
  "kv3.7:1.3_0": "Maṁsacakkhuṁ dibbacakkhuṁ, dibbacakkhuṁ maṁsacakkhunti?",
  "kv3.7:1.4_0": "Na hevaṁ vattabbe …pe….",
  "kv3.7:2.1_0": "Maṁsacakkhuṁ dhammupatthaddhaṁ dibbacakkhuṁ hotīti?",
  "kv3.7:2.2_0": "Āmantā.",
  "kv3.7:2.3_0": "Yādisaṁ maṁsacakkhuṁ tādisaṁ dibbacakkhuṁ, yādisaṁ dibbacakkhuṁ tādisaṁ maṁsacakkhunti?",
  "kv3.7:2.4_0": "Na hevaṁ vattabbe …pe….",
  "kv3.7:3.1_0": "Maṁsacakkhuṁ dhammupatthaddhaṁ dibbacakkhuṁ hotīti?",
  "kv3.7:3.2_0": "Āmantā.",
  "kv3.7:3.3_0": "Taññeva maṁsacakkhuṁ taṁ dibbacakkhuṁ, taṁ dibbacakkhuṁ taṁ maṁsacakkhunti?",
  "kv3.7:3.4_0": "Na hevaṁ vattabbe …pe….",
  "kv3.7:4.1_0": "Maṁsacakkhuṁ dhammupatthaddhaṁ dibbacakkhuṁ hotīti?",
  "kv3.7:4.2_0": "Āmantā.",
  "kv3.7:4.3_0": "Yādiso maṁsacakkhussa visayo ānubhāvo gocaro tādiso dibbassa cakkhussa visayo ānubhāvo gocaroti?",
  "kv3.7:4.4_0": "Na hevaṁ vattabbe …pe….",
  "kv3.7:5.1_0": "Maṁsacakkhuṁ dhammupatthaddhaṁ dibbacakkhuṁ hotīti?",
  "kv3.7:5.2_0": "Āmantā.",
  "kv3.7:5.3_0": "Upādinnaṁ hutvā anupādinnaṁ hotīti?",
  "kv3.7:5.4_0": "Na hevaṁ vattabbe …pe….",
  "kv3.7:6.1_0": "Upādinnaṁ hutvā anupādinnaṁ hotīti?",
  "kv3.7:6.2_0": "Āmantā.",
  "kv3.7:6.3_0": "Kāmāvacaraṁ hutvā rūpāvacaraṁ hotīti?",
  "kv3.7:6.4_0": "Na hevaṁ vattabbe …pe….",
  "kv3.7:7.1_0": "Kāmāvacaraṁ hutvā rūpāvacaraṁ hotīti?",
  "kv3.7:7.2_0": "Āmantā.",
  "kv3.7:7.3_0": "Rūpāvacaraṁ hutvā arūpāvacaraṁ hotīti?",
  "kv3.7:7.4_0": "Na hevaṁ vattabbe …pe….",
  "kv3.7:8.1_0": "Rūpāvacaraṁ hutvā arūpāvacaraṁ hotīti?",
  "kv3.7:8.2_0": "Āmantā.",
  "kv3.7:8.3_0": "Pariyāpannaṁ hutvā apariyāpannaṁ hotīti?",
  "kv3.7:8.4_0": "Na hevaṁ vattabbe …pe….",
  "kv3.7:9.1_0": "Maṁsacakkhuṁ dhammupatthaddhaṁ dibbacakkhuṁ hotīti?",
  "kv3.7:9.2_0": "Āmantā.",
  "kv3.7:9.3_0": "Dibbacakkhuṁ dhammupatthaddhaṁ maṁsacakkhuṁ hotīti?",
  "kv3.7:9.4_0": "Na hevaṁ vattabbe …pe….",
  "kv3.7:10.1_0": "Maṁsacakkhuṁ dhammupatthaddhaṁ dibbacakkhuṁ hotīti?",
  "kv3.7:10.2_0": "Āmantā.",
  "kv3.7:10.3_0": "Dibbacakkhuṁ dhammupatthaddhaṁ paññācakkhuṁ hotīti?",
  "kv3.7:10.4_0": "Na hevaṁ vattabbe …pe….",
  "kv3.7:11.1_0": "Maṁsacakkhuṁ dhammupatthaddhaṁ dibbacakkhuṁ hotīti?",
  "kv3.7:11.2_0": "Āmantā.",
  "kv3.7:11.3_0": "Dibbacakkhuṁ dhammupatthaddhaṁ maṁsacakkhuṁ hotīti?",
  "kv3.7:11.4_0": "Na hevaṁ vattabbe …pe….",
  "kv3.7:12.1_0": "Maṁsacakkhuṁ dhammupatthaddhaṁ dibbacakkhuṁ hotīti?",
  "kv3.7:12.2_0": "Āmantā.",
  "kv3.7:12.3_0": "Dveva cakkhūnīti?",
  "kv3.7:12.4_0": "Na hevaṁ vattabbe …pe…",
  "kv3.7:12.5_0": "dveva cakkhūnīti?",
  "kv3.7:12.6_0": "Āmantā.",
  "kv3.7:12.7_0": "Nanu tīṇi cakkhūni vuttāni bhagavatā—",
  "kv3.7:12.8_0": "maṁsacakkhuṁ, dibbacakkhuṁ, paññācakkhunti?",
  "kv3.7:12.9_0": "Āmantā.",
  "kv3.7:12.10_0": "Hañci tīṇi cakkhūni vuttāni bhagavatā—",
  "kv3.7:12.11_0": "maṁsacakkhuṁ, dibbacakkhuṁ, paññācakkhuṁ, no ca vata re vattabbe—",
  "kv3.7:12.12_0": "“dveva cakkhūnī”ti.",
  "kv3.7:13.1_0": "Dveva cakkhūnīti?",
  "kv3.7:13.2_0": "Āmantā.",
  "kv3.7:13.3_0": "Nanu vuttaṁ bhagavatā—",
  "kv3.7:13.4_0": "“tīṇimāni, bhikkhave, cakkhūni.",
  "kv3.7:13.5_0": "Katamāni tīṇi?",
  "kv3.7:13.6_0": "Maṁsacakkhuṁ, dibbacakkhuṁ, paññācakkhunti—",
  "kv3.7:13.7_0": "imāni kho, bhikkhave, tīṇi cakkhūnī”ti.",
  "kv3.7:14.1_0": "“Maṁsacakkhuṁ dibbacakkhuṁ,",
  "kv3.7:14.2_0": "Paññācakkhuṁ anuttaraṁ;",
  "kv3.7:14.3_0": "Etāni tīṇi cakkhūni,",
  "kv3.7:14.4_0": "Akkhāsi purisuttamo.",
  "kv3.7:15.1_0": "Maṁsacakkhussa uppādo,",
  "kv3.7:15.2_0": "maggo dibbassa cakkhuno;",
  "kv3.7:15.3_0": "Yadā ca ñāṇaṁ udapādi,",
  "kv3.7:15.4_0": "paññācakkhuṁ anuttaraṁ;",
  "kv3.7:15.5_0": "Tassa cakkhussa paṭilābhā,",
  "kv3.7:15.6_0": "sabbadukkhā pamuccatī”ti.",
  "kv3.7:16.1_0": "Attheva suttantoti?",
  "kv3.7:16.2_0": "Āmantā.",
  "kv3.7:16.3_0": "Tena hi na vattabbaṁ—",
  "kv3.7:16.4_0": "“dveva cakkhūnī”ti.",
  "kv3.7:16.5_0": "Dibbacakkhukathā niṭṭhitā."
}