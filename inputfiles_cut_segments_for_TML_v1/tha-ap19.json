{
  "tha-ap19:0.1_0": "Therāpadāna",
  "tha-ap19:0.2_0": "Sīhāsaniyavagga",
  "tha-ap19:0.3_0": "7 Upasenavaṅgantaputtattheraapadāna",
  "tha-ap19:1.1_0": "“Padumuttaraṁ bhagavantaṁ,",
  "tha-ap19:1.2_0": "lokajeṭṭhaṁ narāsabhaṁ;",
  "tha-ap19:1.3_0": "Pabbhāramhi nisīdantaṁ,",
  "tha-ap19:1.4_0": "upagacchiṁ naruttamaṁ.",
  "tha-ap19:2.1_0": "Kaṇikārapupphaṁ disvā,",
  "tha-ap19:2.2_0": "vaṇṭe chetvānahaṁ tadā;",
  "tha-ap19:2.3_0": "Alaṅkaritvā chattamhi,",
  "tha-ap19:2.4_0": "buddhassa abhiropayiṁ.",
  "tha-ap19:3.1_0": "Piṇḍapātañca pādāsiṁ,",
  "tha-ap19:3.2_0": "paramannaṁ subhojanaṁ;",
  "tha-ap19:3.3_0": "Buddhena navame tattha,",
  "tha-ap19:3.4_0": "samaṇe aṭṭha bhojayiṁ.",
  "tha-ap19:4.1_0": "Anumodi mahāvīro,",
  "tha-ap19:4.2_0": "sayambhū aggapuggalo;",
  "tha-ap19:4.3_0": "Iminā chattadānena,",
  "tha-ap19:4.4_0": "paramannapavecchanā.",
  "tha-ap19:5.1_0": "Tena cittappasādena,",
  "tha-ap19:5.2_0": "sampattimanubhossasi;",
  "tha-ap19:5.3_0": "Chattiṁsakkhattuṁ devindo,",
  "tha-ap19:5.4_0": "devarajjaṁ karissati.",
  "tha-ap19:6.1_0": "Ekavīsatikkhattuñca,",
  "tha-ap19:6.2_0": "cakkavattī bhavissati;",
  "tha-ap19:6.3_0": "Padesarajjaṁ vipulaṁ,",
  "tha-ap19:6.4_0": "gaṇanāto asaṅkhiyaṁ.",
  "tha-ap19:7.1_0": "Satasahassito kappe,",
  "tha-ap19:7.2_0": "okkākakulasambhavo;",
  "tha-ap19:7.3_0": "Gotamo nāma gottena,",
  "tha-ap19:7.4_0": "satthā loke bhavissati.",
  "tha-ap19:8.1_0": "Sāsane dibbamānamhi,",
  "tha-ap19:8.2_0": "manussattaṁ gamissati;",
  "tha-ap19:8.3_0": "Tassa dhammesu dāyādo,",
  "tha-ap19:8.4_0": "oraso dhammanimmito.",
  "tha-ap19:9.1_0": "Upasenoti nāmena,",
  "tha-ap19:9.2_0": "hessati satthu sāvako;",
  "tha-ap19:9.3_0": "Samantapāsādikattā,",
  "tha-ap19:9.4_0": "aggaṭṭhāne ṭhapessati.",
  "tha-ap19:10.1_0": "Carimaṁ vattate mayhaṁ,",
  "tha-ap19:10.2_0": "bhavā sabbe samūhatā;",
  "tha-ap19:10.3_0": "Dhāremi antimaṁ dehaṁ,",
  "tha-ap19:10.4_0": "jetvā māraṁ savāhiniṁ.",
  "tha-ap19:11.1_0": "Paṭisambhidā catasso,",
  "tha-ap19:11.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap19:11.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap19:11.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap19:12.1_0": "Itthaṁ sudaṁ āyasmā upaseno vaṅgantaputto thero imā gāthāyo abhāsitthāti.",
  "tha-ap19:13.1_0": "Upasenavaṅgantaputtattherassāpadānaṁ sattamaṁ.",
  "tha-ap19:13.2_0": "Tatiyabhāṇavāraṁ."
}