{
  "tha-ap284:0.1_0": "Therāpadāna",
  "tha-ap284:0.2_0": "Paṇṇadāyakavagga",
  "tha-ap284:0.3_0": "2. Phaladāyakattheraapadāna",
  "tha-ap284:1.1_0": "“Sinerusamasantoso,",
  "tha-ap284:1.2_0": "dharaṇīsamasādiso;",
  "tha-ap284:1.3_0": "Vuṭṭhahitvā samādhimhā,",
  "tha-ap284:1.4_0": "bhikkhāya mamupaṭṭhito.",
  "tha-ap284:2.1_0": "Harītakaṁ āmalakaṁ,",
  "tha-ap284:2.2_0": "ambajambuvibhītakaṁ;",
  "tha-ap284:2.3_0": "Kolaṁ bhallātakaṁ billaṁ,",
  "tha-ap284:2.4_0": "phārusakaphalāni ca.",
  "tha-ap284:3.1_0": "Siddhatthassa mahesissa,",
  "tha-ap284:3.2_0": "sabbalokānukampino;",
  "tha-ap284:3.3_0": "Tañca sabbaṁ mayā dinnaṁ,",
  "tha-ap284:3.4_0": "vippasannena cetasā.",
  "tha-ap284:4.1_0": "Catunnavutito kappe,",
  "tha-ap284:4.2_0": "yaṁ phalaṁ adadiṁ tadā;",
  "tha-ap284:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap284:4.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap284:5.1_0": "Sattapaññāsito kappe,",
  "tha-ap284:5.2_0": "ekajjho nāma khattiyo;",
  "tha-ap284:5.3_0": "Sattaratanasampanno,",
  "tha-ap284:5.4_0": "cakkavattī mahabbalo.",
  "tha-ap284:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap284:6.2_0": "…pe…",
  "tha-ap284:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap284:7.1_0": "Itthaṁ sudaṁ āyasmā phaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap284:8.1_0": "Phaladāyakattherassāpadānaṁ dutiyaṁ."
}