{
  "tha-ap386:0.1_0": "Therāpadāna",
  "tha-ap386:0.2_0": "Avaṭaphalavagga",
  "tha-ap386:0.3_0": "4 Pilakkhaphaladāyakattheraapadāna",
  "tha-ap386:1.1_0": "“Vanantare buddhaṁ disvā,",
  "tha-ap386:1.2_0": "atthadassiṁ mahāyasaṁ;",
  "tha-ap386:1.3_0": "Pasannacitto sumano,",
  "tha-ap386:1.4_0": "pilakkhassādadiṁ phalaṁ.",
  "tha-ap386:2.1_0": "Aṭṭhārase kappasate,",
  "tha-ap386:2.2_0": "yaṁ phalamadadiṁ tadā;",
  "tha-ap386:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap386:2.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap386:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap386:3.2_0": "…pe…",
  "tha-ap386:3.3_0": "viharāmi anāsavo.",
  "tha-ap386:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap386:4.2_0": "…pe…",
  "tha-ap386:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap386:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap386:5.2_0": "…pe…",
  "tha-ap386:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap386:6.1_0": "Itthaṁ sudaṁ āyasmā pilakkhaphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap386:7.1_0": "Pilakkhaphaladāyakattherassāpadānaṁ catutthaṁ."
}