{
  "ja317:0.1_0": "Jātaka",
  "ja317:0.2_0": "Catukkanipāta",
  "ja317:0.3_0": "Pucimandavagga",
  "ja317:0.4_0": "7. Matarodanajātaka",
  "ja317:1.1_0": "“Mataṁ mataṁ evaṁ rodatha,",
  "ja317:1.2_0": "Na hi taṁ rodatha yo marissati;",
  "ja317:1.3_0": "Sabbepi sarīradhārino,",
  "ja317:1.4_0": "Anupubbena jahanti jīvitaṁ.",
  "ja317:2.1_0": "Devamanussā catuppadā,",
  "ja317:2.2_0": "Pakkhigaṇā uragā ca bhogino;",
  "ja317:2.3_0": "Samhi sarīre anissarā,",
  "ja317:2.4_0": "Ramamānāva jahanti jīvitaṁ.",
  "ja317:3.1_0": "Evaṁ calitaṁ asaṇṭhitaṁ,",
  "ja317:3.2_0": "Sukhadukkhaṁ manujesvapekkhiya;",
  "ja317:3.3_0": "Kanditaruditaṁ niratthakaṁ,",
  "ja317:3.4_0": "Kiṁ vo sokagaṇābhikīrare.",
  "ja317:4.1_0": "Dhuttā ca soṇḍā akatā,",
  "ja317:4.2_0": "bālā sūrā ayogino;",
  "ja317:4.3_0": "Dhīraṁ maññanti bāloti,",
  "ja317:4.4_0": "ye dhammassa akovidā”ti.",
  "ja317:5.1_0": "Matarodanajātakaṁ sattamaṁ."
}