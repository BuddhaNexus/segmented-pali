{
  "thag21.1:0.1_0": "Theragāthā",
  "thag21.1:0.2_0": "Mahānipāta",
  "thag21.1:0.3_0": "Paṭhamavagga",
  "thag21.1:0.4_0": "21.1. Vaṅgīsattheragāthā",
  "thag21.1:1.1_0": "“Nikkhantaṁ vata maṁ santaṁ,",
  "thag21.1:1.2_0": "agārasmānagāriyaṁ;",
  "thag21.1:1.3_0": "Vitakkā upadhāvanti,",
  "thag21.1:1.4_0": "pagabbhā kaṇhato ime.",
  "thag21.1:2.1_0": "Uggaputtā mahissāsā,",
  "thag21.1:2.2_0": "sikkhitā daḷhadhammino;",
  "thag21.1:2.3_0": "Samantā parikireyyuṁ,",
  "thag21.1:2.4_0": "sahassaṁ apalāyinaṁ.",
  "thag21.1:3.1_0": "Sacepi ettakā bhiyyo,",
  "thag21.1:3.2_0": "āgamissanti itthiyo;",
  "thag21.1:3.3_0": "Neva maṁ byādhayissanti,",
  "thag21.1:3.4_0": "dhamme samhi patiṭṭhito.",
  "thag21.1:4.1_0": "Sakkhī hi me sutaṁ etaṁ,",
  "thag21.1:4.2_0": "buddhassādiccabandhuno;",
  "thag21.1:4.3_0": "Nibbānagamanaṁ maggaṁ,",
  "thag21.1:4.4_0": "tattha me nirato mano.",
  "thag21.1:5.1_0": "Evañce maṁ viharantaṁ,",
  "thag21.1:5.2_0": "pāpima upagacchasi;",
  "thag21.1:5.3_0": "Tathā maccu karissāmi,",
  "thag21.1:5.4_0": "na me maggampi dakkhasi.",
  "thag21.1:6.1_0": "Aratiñca ratiñca pahāya,",
  "thag21.1:6.2_0": "Sabbaso gehasitañca vitakkaṁ;",
  "thag21.1:6.3_0": "Vanathaṁ na kareyya kuhiñci,",
  "thag21.1:6.4_0": "Nibbanatho avanatho sa bhikkhu.",
  "thag21.1:7.1_0": "Yamidha pathaviñca vehāsaṁ,",
  "thag21.1:7.2_0": "Rūpagataṁ jagatogadhaṁ kiñci;",
  "thag21.1:7.3_0": "Parijīyati sabbamaniccaṁ,",
  "thag21.1:7.4_0": "Evaṁ samecca caranti mutattā.",
  "thag21.1:8.1_0": "Upadhīsu janā gadhitāse,",
  "thag21.1:8.2_0": "Diṭṭhasute paṭighe ca mute ca;",
  "thag21.1:8.3_0": "Ettha vinodaya chandamanejo,",
  "thag21.1:8.4_0": "Yo hettha na limpati muni tamāhu.",
  "thag21.1:9.1_0": "Atha saṭṭhisitā savitakkā,",
  "thag21.1:9.2_0": "Puthujjanatāya adhammā niviṭṭhā;",
  "thag21.1:9.3_0": "Na ca vaggagatassa kuhiñci,",
  "thag21.1:9.4_0": "No pana duṭṭhullagāhī sa bhikkhu.",
  "thag21.1:10.1_0": "Dabbo cirarattasamāhito,",
  "thag21.1:10.2_0": "Akuhako nipako apihālu;",
  "thag21.1:10.3_0": "Santaṁ padaṁ ajjhagamā muni,",
  "thag21.1:10.4_0": "Paṭicca parinibbuto kaṅkhati kālaṁ.",
  "thag21.1:11.1_0": "Mānaṁ pajahassu gotama,",
  "thag21.1:11.2_0": "Mānapathañca jahassu asesaṁ;",
  "thag21.1:11.3_0": "Mānapathamhi sa mucchito,",
  "thag21.1:11.4_0": "Vippaṭisārīhuvā cirarattaṁ.",
  "thag21.1:12.1_0": "Makkhena makkhitā pajā,",
  "thag21.1:12.2_0": "Mānahatā nirayaṁ papatanti;",
  "thag21.1:12.3_0": "Socanti janā cirarattaṁ,",
  "thag21.1:12.4_0": "Mānahatā nirayaṁ upapannā.",
  "thag21.1:13.1_0": "Na hi socati bhikkhu kadāci,",
  "thag21.1:13.2_0": "Maggajino sammā paṭipanno;",
  "thag21.1:13.3_0": "Kittiñca sukhañcānubhoti,",
  "thag21.1:13.4_0": "Dhammadasoti tamāhu tathattaṁ.",
  "thag21.1:14.1_0": "Tasmā akhilo idha padhānavā,",
  "thag21.1:14.2_0": "Nīvaraṇāni pahāya visuddho;",
  "thag21.1:14.3_0": "Mānañca pahāya asesaṁ,",
  "thag21.1:14.4_0": "Vijjāyantakaro samitāvī”.",
  "thag21.1:15.1_0": "“Kāmarāgena ḍayhāmi,",
  "thag21.1:15.2_0": "cittaṁ me pariḍayhati;",
  "thag21.1:15.3_0": "Sādhu nibbāpanaṁ brūhi,",
  "thag21.1:15.4_0": "anukampāya gotama”.",
  "thag21.1:16.1_0": "“Saññāya vipariyesā,",
  "thag21.1:16.2_0": "cittaṁ te pariḍayhati;",
  "thag21.1:16.3_0": "Nimittaṁ parivajjehi,",
  "thag21.1:16.4_0": "subhaṁ rāgūpasaṁhitaṁ. (…)",
  "thag21.1:17.1_0": "Asubhāya cittaṁ bhāvehi,",
  "thag21.1:17.2_0": "ekaggaṁ susamāhitaṁ;",
  "thag21.1:17.3_0": "Sati kāyagatā tyatthu,",
  "thag21.1:17.4_0": "nibbidābahulo bhava.",
  "thag21.1:18.1_0": "Animittañca bhāvehi,",
  "thag21.1:18.2_0": "mānānusayamujjaha;",
  "thag21.1:18.3_0": "Tato mānābhisamayā,",
  "thag21.1:18.4_0": "upasanto carissasi”.",
  "thag21.1:19.1_0": "“Tameva vācaṁ bhāseyya,",
  "thag21.1:19.2_0": "yāyattānaṁ na tāpaye;",
  "thag21.1:19.3_0": "Pare ca na vihiṁseyya,",
  "thag21.1:19.4_0": "sā ve vācā subhāsitā.",
  "thag21.1:20.1_0": "Piyavācameva bhāseyya,",
  "thag21.1:20.2_0": "yā vācā paṭinanditā;",
  "thag21.1:20.3_0": "Yaṁ anādāya pāpāni,",
  "thag21.1:20.4_0": "paresaṁ bhāsate piyaṁ.",
  "thag21.1:21.1_0": "Saccaṁ ve amatā vācā,",
  "thag21.1:21.2_0": "esa dhammo sanantano;",
  "thag21.1:21.3_0": "Sacce atthe ca dhamme ca,",
  "thag21.1:21.4_0": "āhu santo patiṭṭhitā.",
  "thag21.1:22.1_0": "Yaṁ buddho bhāsati vācaṁ,",
  "thag21.1:22.2_0": "khemaṁ nibbānapattiyā;",
  "thag21.1:22.3_0": "Dukkhassantakiriyāya,",
  "thag21.1:22.4_0": "sā ve vācānamuttamā”.",
  "thag21.1:23.1_0": "“Gambhīrapañño medhāvī,",
  "thag21.1:23.2_0": "maggāmaggassa kovido;",
  "thag21.1:23.3_0": "Sāriputto mahāpañño,",
  "thag21.1:23.4_0": "dhammaṁ deseti bhikkhunaṁ.",
  "thag21.1:24.1_0": "Saṅkhittenapi deseti,",
  "thag21.1:24.2_0": "vitthārenapi bhāsati;",
  "thag21.1:24.3_0": "Sālikāyiva nigghoso,",
  "thag21.1:24.4_0": "paṭibhānaṁ udiyyati.",
  "thag21.1:25.1_0": "Tassa taṁ desayantassa,",
  "thag21.1:25.2_0": "suṇanti madhuraṁ giraṁ;",
  "thag21.1:25.3_0": "Sarena rajanīyena,",
  "thag21.1:25.4_0": "savanīyena vaggunā;",
  "thag21.1:25.5_0": "Udaggacittā muditā,",
  "thag21.1:25.6_0": "sotaṁ odhenti bhikkhavo”.",
  "thag21.1:26.1_0": "“Ajja pannarase visuddhiyā,",
  "thag21.1:26.2_0": "Bhikkhū pañcasatā samāgatā;",
  "thag21.1:26.3_0": "Saṁyojanabandhanacchidā,",
  "thag21.1:26.4_0": "Anīghā khīṇapunabbhavā isī.",
  "thag21.1:27.1_0": "Cakkavattī yathā rājā,",
  "thag21.1:27.2_0": "amaccaparivārito;",
  "thag21.1:27.3_0": "Samantā anupariyeti,",
  "thag21.1:27.4_0": "sāgarantaṁ mahiṁ imaṁ.",
  "thag21.1:28.1_0": "Evaṁ vijitasaṅgāmaṁ,",
  "thag21.1:28.2_0": "satthavāhaṁ anuttaraṁ;",
  "thag21.1:28.3_0": "Sāvakā payirupāsanti,",
  "thag21.1:28.4_0": "tevijjā maccuhāyino.",
  "thag21.1:29.1_0": "Sabbe bhagavato puttā,",
  "thag21.1:29.2_0": "palāpettha na vijjati;",
  "thag21.1:29.3_0": "Taṇhāsallassa hantāraṁ,",
  "thag21.1:29.4_0": "vande ādiccabandhunaṁ.",
  "thag21.1:30.1_0": "Parosahassaṁ bhikkhūnaṁ,",
  "thag21.1:30.2_0": "sugataṁ payirupāsati;",
  "thag21.1:30.3_0": "Desentaṁ virajaṁ dhammaṁ,",
  "thag21.1:30.4_0": "nibbānaṁ akutobhayaṁ.",
  "thag21.1:31.1_0": "Suṇanti dhammaṁ vimalaṁ,",
  "thag21.1:31.2_0": "sammāsambuddhadesitaṁ;",
  "thag21.1:31.3_0": "Sobhati vata sambuddho,",
  "thag21.1:31.4_0": "bhikkhusaṅghapurakkhato.",
  "thag21.1:32.1_0": "‘Nāganāmo’si bhagavā,",
  "thag21.1:32.2_0": "isīnaṁ isisattamo;",
  "thag21.1:32.3_0": "Mahāmeghova hutvāna,",
  "thag21.1:32.4_0": "sāvake abhivassasi.",
  "thag21.1:33.1_0": "Divā vihārā nikkhamma,",
  "thag21.1:33.2_0": "satthudassanakamyatā;",
  "thag21.1:33.3_0": "Sāvako te mahāvīra,",
  "thag21.1:33.4_0": "pāde vandati vaṅgiso”.",
  "thag21.1:34.1_0": "“Ummaggapathaṁ mārassa,",
  "thag21.1:34.2_0": "Abhibhuyya carati pabhijja khīlāni;",
  "thag21.1:34.3_0": "Taṁ passatha bandhapamuñcakaraṁ,",
  "thag21.1:34.4_0": "Asitaṁva bhāgaso pavibhajja.",
  "thag21.1:35.1_0": "Oghassa hi nitaraṇatthaṁ,",
  "thag21.1:35.2_0": "Anekavihitaṁ maggaṁ akkhāsi;",
  "thag21.1:35.3_0": "Tasmiñca amate akkhāte,",
  "thag21.1:35.4_0": "Dhammadasā ṭhitā asaṁhīrā.",
  "thag21.1:36.1_0": "Pajjotakaro ativijjha,",
  "thag21.1:36.2_0": "Sabbaṭhitīnaṁ atikkamamaddasa;",
  "thag21.1:36.3_0": "Ñatvā ca sacchikatvā ca,",
  "thag21.1:36.4_0": "Aggaṁ so desayi dasaddhānaṁ.",
  "thag21.1:37.1_0": "Evaṁ sudesite dhamme,",
  "thag21.1:37.2_0": "Ko pamādo vijānataṁ dhammaṁ;",
  "thag21.1:37.3_0": "Tasmā hi tassa bhagavato sāsane,",
  "thag21.1:37.4_0": "Appamatto sadā namassamanusikkhe”.",
  "thag21.1:38.1_0": "“Buddhānubuddho yo thero,",
  "thag21.1:38.2_0": "koṇḍañño tibbanikkamo;",
  "thag21.1:38.3_0": "Lābhī sukhavihārānaṁ,",
  "thag21.1:38.4_0": "vivekānaṁ abhiṇhaso.",
  "thag21.1:39.1_0": "Yaṁ sāvakena pattabbaṁ,",
  "thag21.1:39.2_0": "satthu sāsanakārinā;",
  "thag21.1:39.3_0": "Sabbassa taṁ anuppattaṁ,",
  "thag21.1:39.4_0": "appamattassa sikkhato.",
  "thag21.1:40.1_0": "Mahānubhāvo tevijjo,",
  "thag21.1:40.2_0": "cetopariyakovido;",
  "thag21.1:40.3_0": "Koṇḍañño buddhadāyādo,",
  "thag21.1:40.4_0": "pāde vandati satthuno”.",
  "thag21.1:41.1_0": "“Nagassa passe āsīnaṁ,",
  "thag21.1:41.2_0": "muniṁ dukkhassa pāraguṁ;",
  "thag21.1:41.3_0": "Sāvakā payirupāsanti,",
  "thag21.1:41.4_0": "tevijjā maccuhāyino.",
  "thag21.1:42.1_0": "Cetasā anupariyeti,",
  "thag21.1:42.2_0": "moggallāno mahiddhiko;",
  "thag21.1:42.3_0": "Cittaṁ nesaṁ samanvesaṁ,",
  "thag21.1:42.4_0": "vippamuttaṁ nirūpadhiṁ.",
  "thag21.1:43.1_0": "Evaṁ sabbaṅgasampannaṁ,",
  "thag21.1:43.2_0": "muniṁ dukkhassa pāraguṁ;",
  "thag21.1:43.3_0": "Anekākārasampannaṁ,",
  "thag21.1:43.4_0": "payirupāsanti gotamaṁ”.",
  "thag21.1:44.1_0": "“Cando yathā vigatavalāhake nabhe,",
  "thag21.1:44.2_0": "Virocati vītamalova bhāṇumā;",
  "thag21.1:44.3_0": "Evampi aṅgīrasa tvaṁ mahāmuni,",
  "thag21.1:44.4_0": "Atirocasi yasasā sabbalokaṁ”.",
  "thag21.1:45.1_0": "“Kāveyyamattā vicarimha pubbe,",
  "thag21.1:45.2_0": "Gāmā gāmaṁ purā puraṁ;",
  "thag21.1:45.3_0": "Athaddasāma sambuddhaṁ,",
  "thag21.1:45.4_0": "Sabbadhammāna pāraguṁ.",
  "thag21.1:46.1_0": "So me dhammamadesesi,",
  "thag21.1:46.2_0": "muni dukkhassa pāragū;",
  "thag21.1:46.3_0": "Dhammaṁ sutvā pasīdimha,",
  "thag21.1:46.4_0": "saddhā no udapajjatha.",
  "thag21.1:47.1_0": "Tassāhaṁ vacanaṁ sutvā,",
  "thag21.1:47.2_0": "khandhe āyatanāni ca;",
  "thag21.1:47.3_0": "Dhātuyo ca viditvāna,",
  "thag21.1:47.4_0": "pabbajiṁ anagāriyaṁ.",
  "thag21.1:48.1_0": "Bahūnaṁ vata atthāya,",
  "thag21.1:48.2_0": "uppajjanti tathāgatā;",
  "thag21.1:48.3_0": "Itthīnaṁ purisānañca,",
  "thag21.1:48.4_0": "ye te sāsanakārakā.",
  "thag21.1:49.1_0": "Tesaṁ kho vata atthāya,",
  "thag21.1:49.2_0": "bodhimajjhagamā muni;",
  "thag21.1:49.3_0": "Bhikkhūnaṁ bhikkhunīnañca,",
  "thag21.1:49.4_0": "ye nirāmagataddasā.",
  "thag21.1:50.1_0": "Sudesitā cakkhumatā,",
  "thag21.1:50.2_0": "buddhenādiccabandhunā;",
  "thag21.1:50.3_0": "Cattāri ariyasaccāni,",
  "thag21.1:50.4_0": "anukampāya pāṇinaṁ.",
  "thag21.1:51.1_0": "Dukkhaṁ dukkhasamuppādaṁ,",
  "thag21.1:51.2_0": "Dukkhassa ca atikkamaṁ;",
  "thag21.1:51.3_0": "Ariyaṁ caṭṭhaṅgikaṁ maggaṁ,",
  "thag21.1:51.4_0": "Dukkhūpasamagāminaṁ.",
  "thag21.1:52.1_0": "Evamete tathā vuttā,",
  "thag21.1:52.2_0": "diṭṭhā me te yathā tathā;",
  "thag21.1:52.3_0": "Sadattho me anuppatto,",
  "thag21.1:52.4_0": "kataṁ buddhassa sāsanaṁ.",
  "thag21.1:53.1_0": "Svāgataṁ vata me āsi,",
  "thag21.1:53.2_0": "mama buddhassa santike;",
  "thag21.1:53.3_0": "Suvibhattesu dhammesu,",
  "thag21.1:53.4_0": "yaṁ seṭṭhaṁ tadupāgamiṁ.",
  "thag21.1:54.1_0": "Abhiññāpāramippatto,",
  "thag21.1:54.2_0": "sotadhātu visodhitā;",
  "thag21.1:54.3_0": "Tevijjo iddhipattomhi,",
  "thag21.1:54.4_0": "cetopariyakovido”.",
  "thag21.1:55.1_0": "“Pucchāmi satthāramanomapaññaṁ,",
  "thag21.1:55.2_0": "Diṭṭheva dhamme yo vicikicchānaṁ chettā;",
  "thag21.1:55.3_0": "Aggāḷave kālamakāsi bhikkhu,",
  "thag21.1:55.4_0": "Ñāto yasassī abhinibbutatto.",
  "thag21.1:56.1_0": "Nigrodhakappo iti tassa nāmaṁ,",
  "thag21.1:56.2_0": "Tayā kataṁ bhagavā brāhmaṇassa;",
  "thag21.1:56.3_0": "So taṁ namassaṁ acari mutyapekho,",
  "thag21.1:56.4_0": "Āraddhavīriyo daḷhadhammadassī.",
  "thag21.1:57.1_0": "Taṁ sāvakaṁ sakka mayampi sabbe,",
  "thag21.1:57.2_0": "Aññātumicchāma samantacakkhu;",
  "thag21.1:57.3_0": "Samavaṭṭhitā no savanāya sotā,",
  "thag21.1:57.4_0": "Tuvaṁ no satthā tvamanuttarosi.",
  "thag21.1:58.1_0": "Chinda no vicikicchaṁ brūhi metaṁ,",
  "thag21.1:58.2_0": "Parinibbutaṁ vedaya bhūripañña;",
  "thag21.1:58.3_0": "Majjheva no bhāsa samantacakkhu,",
  "thag21.1:58.4_0": "Sakkova devāna sahassanetto.",
  "thag21.1:59.1_0": "Ye keci ganthā idha mohamaggā,",
  "thag21.1:59.2_0": "Aññāṇapakkhā vicikicchaṭhānā;",
  "thag21.1:59.3_0": "Tathāgataṁ patvā na te bhavanti,",
  "thag21.1:59.4_0": "Cakkhuñhi etaṁ paramaṁ narānaṁ.",
  "thag21.1:60.1_0": "No ce hi jātu puriso kilese,",
  "thag21.1:60.2_0": "Vāto yathā abbhaghanaṁ vihāne;",
  "thag21.1:60.3_0": "Tamovassa nivuto sabbaloko,",
  "thag21.1:60.4_0": "Jotimantopi na pabhāseyyuṁ.",
  "thag21.1:61.1_0": "Dhīrā ca pajjotakarā bhavanti,",
  "thag21.1:61.2_0": "Taṁ taṁ ahaṁ vīra tatheva maññe;",
  "thag21.1:61.3_0": "Vipassinaṁ jānamupāgamimha,",
  "thag21.1:61.4_0": "Parisāsu no āvikarohi kappaṁ.",
  "thag21.1:62.1_0": "Khippaṁ giraṁ eraya vaggu vagguṁ,",
  "thag21.1:62.2_0": "Haṁsova paggayha saṇikaṁ nikūja;",
  "thag21.1:62.3_0": "Bindussarena suvikappitena,",
  "thag21.1:62.4_0": "Sabbeva te ujjugatā suṇoma.",
  "thag21.1:63.1_0": "Pahīnajātimaraṇaṁ asesaṁ,",
  "thag21.1:63.2_0": "Niggayha dhonaṁ vadessāmi dhammaṁ;",
  "thag21.1:63.3_0": "Na kāmakāro hi puthujjanānaṁ,",
  "thag21.1:63.4_0": "Saṅkheyyakāro ca tathāgatānaṁ.",
  "thag21.1:64.1_0": "Sampannaveyyākaraṇaṁ tavedaṁ,",
  "thag21.1:64.2_0": "Samujjupaññassa samuggahītaṁ;",
  "thag21.1:64.3_0": "Ayamañjali pacchimo suppaṇāmito,",
  "thag21.1:64.4_0": "Mā mohayī jānamanomapañña.",
  "thag21.1:65.1_0": "Paroparaṁ ariyadhammaṁ viditvā,",
  "thag21.1:65.2_0": "Mā mohayī jānamanomavīriya;",
  "thag21.1:65.3_0": "Vāriṁ yathā ghammani ghammatatto,",
  "thag21.1:65.4_0": "Vācābhikaṅkhāmi sutaṁ pavassa.",
  "thag21.1:66.1_0": "Yadatthikaṁ brahmacariyaṁ acarī,",
  "thag21.1:66.2_0": "Kappāyano kaccissataṁ amoghaṁ;",
  "thag21.1:66.3_0": "Nibbāyi so ādu saupādiseso,",
  "thag21.1:66.4_0": "Yathā vimutto ahu taṁ suṇoma”.",
  "thag21.1:67.1_0": "“Acchecchi taṇhaṁ idha nāmarūpe,",
  "thag21.1:67.2_0": "(iti bhagavā)",
  "thag21.1:67.3_0": "Kaṇhassa sotaṁ dīgharattānusayitaṁ;",
  "thag21.1:67.4_0": "Atāri jātiṁ maraṇaṁ asesaṁ,",
  "thag21.1:67.5_0": "Iccabravi bhagavā pañcaseṭṭho”.",
  "thag21.1:68.1_0": "“Esa sutvā pasīdāmi,",
  "thag21.1:68.2_0": "vaco te isisattama;",
  "thag21.1:68.3_0": "Amoghaṁ kira me puṭṭhaṁ,",
  "thag21.1:68.4_0": "na maṁ vañcesi brāhmaṇo.",
  "thag21.1:69.1_0": "Yathā vādī tathā kārī,",
  "thag21.1:69.2_0": "ahu buddhassa sāvako;",
  "thag21.1:69.3_0": "Acchecchi maccuno jālaṁ,",
  "thag21.1:69.4_0": "tataṁ māyāvino daḷhaṁ.",
  "thag21.1:70.1_0": "Addasa bhagavā ādiṁ,",
  "thag21.1:70.2_0": "upādānassa kappiyo;",
  "thag21.1:70.3_0": "Accagā vata kappāno,",
  "thag21.1:70.4_0": "maccudheyyaṁ suduttaraṁ.",
  "thag21.1:71.1_0": "Taṁ devadevaṁ vandāmi,",
  "thag21.1:71.2_0": "puttaṁ te dvipaduttama;",
  "thag21.1:71.3_0": "Anujātaṁ mahāvīraṁ,",
  "thag21.1:71.4_0": "nāgaṁ nāgassa orasan”ti.",
  "thag21.1:71.5_0": "Itthaṁ sudaṁ āyasmā vaṅgīso thero gāthāyo abhāsitthāti.",
  "thag21.1:71.6_0": "Mahānipāto niṭṭhito.",
  "thag21.1:72.0_0": "Tatruddānaṁ",
  "thag21.1:72.1_0": "Sattatimhi nipātamhi,",
  "thag21.1:72.2_0": "vaṅgīso paṭibhāṇavā;",
  "thag21.1:72.3_0": "Ekova thero natthañño,",
  "thag21.1:72.4_0": "gāthāyo ekasattatīti.",
  "thag21.1:72.5_0": "Niṭṭhitā theragāthāyo.",
  "thag21.1:73.0_0": "Tatruddānaṁ",
  "thag21.1:73.1_0": "Sahassaṁ honti tā gāthā,",
  "thag21.1:73.2_0": "tīṇi saṭṭhisatāni ca;",
  "thag21.1:73.3_0": "Therā ca dve satā saṭṭhi,",
  "thag21.1:73.4_0": "cattāro ca pakāsitā.",
  "thag21.1:74.1_0": "Sīhanādaṁ naditvāna,",
  "thag21.1:74.2_0": "buddhaputtā anāsavā;",
  "thag21.1:74.3_0": "Khemantaṁ pāpuṇitvāna,",
  "thag21.1:74.4_0": "aggikhandhāva nibbutāti.",
  "thag21.1:74.5_0": "Theragāthāpāḷi niṭṭhitā."
}