{
  "thag1.28:0.1_0": "Theragāthā",
  "thag1.28:0.2_0": "Ekakanipāta",
  "thag1.28:0.3_0": "Tatiyavagga",
  "thag1.28:0.4_0": "1.28 Jambugāmikaputtattheragāthā",
  "thag1.28:1.1_0": "“Kacci no vatthapasuto,",
  "thag1.28:1.2_0": "Kacci no bhūsanārato;",
  "thag1.28:1.3_0": "Kacci sīlamayaṁ gandhaṁ,",
  "thag1.28:1.4_0": "Kiṁ tvaṁ vāyasi netarā pajā”ti.",
  "thag1.28:1.5_0": "… Jambugāmikaputto thero …."
}