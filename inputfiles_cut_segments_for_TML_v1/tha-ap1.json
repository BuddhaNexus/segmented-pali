{
  "tha-ap1:0.1_0": "Therāpadāna",
  "tha-ap1:0.2_0": "Buddhavagga",
  "tha-ap1:0.3_0": "1. Buddhaapadāna",
  "tha-ap1:1.1_0": "Namo tassa Bhagavato Arahato Sammāsambuddhassa.",
  "tha-ap1:2.1_0": "Tathāgataṁ jetavane vasantaṁ,",
  "tha-ap1:2.2_0": "Apucchi vedehamunī nataṅgo;",
  "tha-ap1:2.3_0": "“Sabbaññubuddhā kira nāma honti,",
  "tha-ap1:2.4_0": "Bhavanti te hetubhi kehi vīra”.",
  "tha-ap1:3.1_0": "Tadāha sabbaññuvaro mahesī,",
  "tha-ap1:3.2_0": "Ānandabhaddaṁ madhurassarena;",
  "tha-ap1:3.3_0": "“Ye pubbabuddhesu katādhikārā,",
  "tha-ap1:3.4_0": "Aladdhamokkhā jinasāsanesu.",
  "tha-ap1:4.1_0": "Teneva sambodhimukhena dhīrā,",
  "tha-ap1:4.2_0": "Ajjhāsayenāpi mahābalena;",
  "tha-ap1:4.3_0": "Paññāya tejena sutikkhapaññā,",
  "tha-ap1:4.4_0": "Sabbaññubhāvaṁ anupāpuṇanti.",
  "tha-ap1:5.1_0": "Ahampi pubbabuddhesu,",
  "tha-ap1:5.2_0": "buddhattamabhipatthayiṁ;",
  "tha-ap1:5.3_0": "Manasāyeva hutvāna,",
  "tha-ap1:5.4_0": "dhammarājā asaṅkhiyā.",
  "tha-ap1:6.1_0": "Atha buddhāpadānāni,",
  "tha-ap1:6.2_0": "suṇātha suddhamānasā;",
  "tha-ap1:6.3_0": "Tiṁsapāramisampuṇṇā,",
  "tha-ap1:6.4_0": "dhammarājā asaṅkhiyā.",
  "tha-ap1:7.1_0": "Sambodhiṁ buddhaseṭṭhānaṁ,",
  "tha-ap1:7.2_0": "sasaṅghe lokanāyake;",
  "tha-ap1:7.3_0": "Dasaṅgulī namassitvā,",
  "tha-ap1:7.4_0": "sirasā abhivādayiṁ.",
  "tha-ap1:8.1_0": "Yāvatā buddhakhettesu,",
  "tha-ap1:8.2_0": "ratanā vijjantisaṅkhiyā;",
  "tha-ap1:8.3_0": "Ākāsaṭṭhā ca bhūmaṭṭhā,",
  "tha-ap1:8.4_0": "manasā sabbamāhariṁ.",
  "tha-ap1:9.1_0": "Tattha rūpiyabhūmiyaṁ,",
  "tha-ap1:9.2_0": "pāsādaṁ māpayiṁ ahaṁ;",
  "tha-ap1:9.3_0": "Nekabhummaṁ ratanāmayaṁ,",
  "tha-ap1:9.4_0": "ubbiddhaṁ nabhamuggataṁ.",
  "tha-ap1:10.1_0": "Vicittathambhaṁ sukataṁ,",
  "tha-ap1:10.2_0": "suvibhattaṁ mahārahaṁ;",
  "tha-ap1:10.3_0": "Kanakamayasaṅghāṭaṁ,",
  "tha-ap1:10.4_0": "kontacchattehi maṇḍitaṁ.",
  "tha-ap1:11.1_0": "Paṭhamā veḷuriyā bhūmi,",
  "tha-ap1:11.2_0": "vimalabbhasamā subhā;",
  "tha-ap1:11.3_0": "Naḷinajalajākiṇṇā,",
  "tha-ap1:11.4_0": "varakañcanabhūmiyā.",
  "tha-ap1:12.1_0": "Pavāḷaṁsā pavāḷavaṇṇā,",
  "tha-ap1:12.2_0": "kāci lohitakā subhā;",
  "tha-ap1:12.3_0": "Indagopakavaṇṇābhā,",
  "tha-ap1:12.4_0": "bhūmi obhāsatī disā.",
  "tha-ap1:13.1_0": "Suvibhattā gharamukhā,",
  "tha-ap1:13.2_0": "niyyūhā sīhapañjarā;",
  "tha-ap1:13.3_0": "Caturo vedikā jālā,",
  "tha-ap1:13.4_0": "gandhāveḷā manoramā.",
  "tha-ap1:14.1_0": "Nīlā pītā lohitakā,",
  "tha-ap1:14.2_0": "odātā suddhakāḷakā;",
  "tha-ap1:14.3_0": "Kūṭāgāravarūpetā,",
  "tha-ap1:14.4_0": "sattaratanabhūsitā.",
  "tha-ap1:15.1_0": "Olokamayā padumā,",
  "tha-ap1:15.2_0": "vāḷavihaṅgasobhitā;",
  "tha-ap1:15.3_0": "Nakkhattatārakākiṇṇā,",
  "tha-ap1:15.4_0": "candasūrehi maṇḍitā.",
  "tha-ap1:16.1_0": "Hemajālena sañchannā,",
  "tha-ap1:16.2_0": "soṇṇakiṅkiṇikāyutā;",
  "tha-ap1:16.3_0": "Vātavegena kūjanti,",
  "tha-ap1:16.4_0": "soṇṇamālā manoramā.",
  "tha-ap1:17.1_0": "Mañjeṭṭhakaṁ lohitakaṁ,",
  "tha-ap1:17.2_0": "pītakaṁ haripiñjaraṁ;",
  "tha-ap1:17.3_0": "Nānāraṅgehi sampītaṁ,",
  "tha-ap1:17.4_0": "ussitaddhajamālinī.",
  "tha-ap1:18.1_0": "Na naṁ bahūnekasatā,",
  "tha-ap1:18.2_0": "phalikā rajatāmayā;",
  "tha-ap1:18.3_0": "Maṇimayā lohitaṅgā,",
  "tha-ap1:18.4_0": "masāragallamayā tathā;",
  "tha-ap1:18.5_0": "Nānāsayanavicittā,",
  "tha-ap1:18.6_0": "saṇhakāsikasanthatā.",
  "tha-ap1:19.1_0": "Kampalā dukūlā cīnā,",
  "tha-ap1:19.2_0": "pattuṇṇā paṇḍupāvurā;",
  "tha-ap1:19.3_0": "Vividhattharaṇaṁ sabbaṁ,",
  "tha-ap1:19.4_0": "manasā paññapesahaṁ.",
  "tha-ap1:20.1_0": "Tāsu tāsveva bhūmīsu,",
  "tha-ap1:20.2_0": "ratanakūṭalaṅkataṁ;",
  "tha-ap1:20.3_0": "Maṇiverocanā ukkā,",
  "tha-ap1:20.4_0": "dhārayantā sutiṭṭhare.",
  "tha-ap1:21.1_0": "Sobhanti esikā thambhā,",
  "tha-ap1:21.2_0": "subhā kañcanatoraṇā;",
  "tha-ap1:21.3_0": "Jambonadā sāramayā,",
  "tha-ap1:21.4_0": "atho rajatamayāpi ca.",
  "tha-ap1:22.1_0": "Nekā sandhī suvibhattā,",
  "tha-ap1:22.2_0": "kavāṭaggaḷacittitā;",
  "tha-ap1:22.3_0": "Ubhato puṇṇaghaṭānekā,",
  "tha-ap1:22.4_0": "padumuppalasaṁyutā.",
  "tha-ap1:23.1_0": "Atīte sabbabuddhe ca,",
  "tha-ap1:23.2_0": "sasaṅghe lokanāyake;",
  "tha-ap1:23.3_0": "Pakativaṇṇarūpena,",
  "tha-ap1:23.4_0": "nimminitvā sasāvake.",
  "tha-ap1:24.1_0": "Tena dvārena pavisitvā,",
  "tha-ap1:24.2_0": "sabbe buddhā sasāvakā;",
  "tha-ap1:24.3_0": "Sabbasoṇṇamaye pīṭhe,",
  "tha-ap1:24.4_0": "nisinnā ariyamaṇḍalā.",
  "tha-ap1:25.1_0": "Ye ca etarahi atthi,",
  "tha-ap1:25.2_0": "Buddhā loke anuttarā;",
  "tha-ap1:25.3_0": "Atīte vattamānā ca,",
  "tha-ap1:25.4_0": "Bhavanaṁ sabbe samāruhuṁ.",
  "tha-ap1:26.1_0": "Paccekabuddhenekasate,",
  "tha-ap1:26.2_0": "Sayambhū aparājite;",
  "tha-ap1:26.3_0": "Atīte vattamāne ca,",
  "tha-ap1:26.4_0": "Bhavanaṁ sabbe samāruhuṁ.",
  "tha-ap1:27.1_0": "Kapparukkhā bahū atthi,",
  "tha-ap1:27.2_0": "Ye dibbā ye ca mānusā;",
  "tha-ap1:27.3_0": "Sabbaṁ dussaṁ samāhantvā,",
  "tha-ap1:27.4_0": "Acchādemi ticīvaraṁ.",
  "tha-ap1:28.1_0": "Khajjaṁ bhojjaṁ sāyanīyaṁ,",
  "tha-ap1:28.2_0": "sampannaṁ pānabhojanaṁ;",
  "tha-ap1:28.3_0": "Maṇimaye subhe patte,",
  "tha-ap1:28.4_0": "sampūretvā adāsahaṁ.",
  "tha-ap1:29.1_0": "Dibbavatthasamā hutvā,",
  "tha-ap1:29.2_0": "maṭṭhā cīvarasaṁyutā;",
  "tha-ap1:29.3_0": "Madhurā sakkharā ceva,",
  "tha-ap1:29.4_0": "telā ca madhuphāṇitā.",
  "tha-ap1:30.1_0": "Tappitā paramannena,",
  "tha-ap1:30.2_0": "sabbe te ariyamaṇḍalā;",
  "tha-ap1:30.3_0": "Ratanagabbhaṁ pavisitvā,",
  "tha-ap1:30.4_0": "kesarīva guhāsayā.",
  "tha-ap1:31.1_0": "Mahārahamhi sayane,",
  "tha-ap1:31.2_0": "sīhaseyyamakappayuṁ;",
  "tha-ap1:31.3_0": "Sampajānā samuṭṭhāya,",
  "tha-ap1:31.4_0": "sayane pallaṅkamābhujuṁ.",
  "tha-ap1:32.1_0": "Gocaraṁ sabbabuddhānaṁ,",
  "tha-ap1:32.2_0": "jhānaratisamappitā;",
  "tha-ap1:32.3_0": "Aññe dhammāni desenti,",
  "tha-ap1:32.4_0": "aññe kīḷanti iddhiyā.",
  "tha-ap1:33.1_0": "Aññe abhiññā appenti,",
  "tha-ap1:33.2_0": "abhiññā vasibhāvitā;",
  "tha-ap1:33.3_0": "Vikubbanā vikubbanti,",
  "tha-ap1:33.4_0": "aññenekasahassiyo.",
  "tha-ap1:34.1_0": "Buddhāpi buddhe pucchanti,",
  "tha-ap1:34.2_0": "visayaṁ sabbaññumālayaṁ;",
  "tha-ap1:34.3_0": "Gambhīraṁ nipuṇaṁ ṭhānaṁ,",
  "tha-ap1:34.4_0": "paññāya vinibujjhare.",
  "tha-ap1:35.1_0": "Sāvakā buddhe pucchanti,",
  "tha-ap1:35.2_0": "buddhā pucchanti sāvake;",
  "tha-ap1:35.3_0": "Aññamaññañca pucchitvā,",
  "tha-ap1:35.4_0": "aññoññaṁ byākaronti te.",
  "tha-ap1:36.1_0": "Buddhā paccekabuddhā ca,",
  "tha-ap1:36.2_0": "sāvakā paricārakā;",
  "tha-ap1:36.3_0": "Evaṁ sakāya ratiyā,",
  "tha-ap1:36.4_0": "pāsādebhiramanti te.",
  "tha-ap1:37.1_0": "Chattā tiṭṭhantu ratanā,",
  "tha-ap1:37.2_0": "kañcanāveḷapantikā;",
  "tha-ap1:37.3_0": "Muttājālaparikkhittā,",
  "tha-ap1:37.4_0": "sabbe dhārentu matthake.",
  "tha-ap1:38.1_0": "Bhavantu ceḷavitānā,",
  "tha-ap1:38.2_0": "soṇṇatārakacittitā;",
  "tha-ap1:38.3_0": "Vicittamalyavitatā,",
  "tha-ap1:38.4_0": "sabbe dhārentu matthake.",
  "tha-ap1:39.1_0": "Vitatā malyadāmehi,",
  "tha-ap1:39.2_0": "gandhadāmehi sobhitā;",
  "tha-ap1:39.3_0": "Dussadāmaparikiṇṇā,",
  "tha-ap1:39.4_0": "ratanadāmabhūsitā.",
  "tha-ap1:40.1_0": "Pupphābhikiṇṇā sucittā,",
  "tha-ap1:40.2_0": "surabhigandhabhūsitā;",
  "tha-ap1:40.3_0": "Gandhapañcaṅgulakatā,",
  "tha-ap1:40.4_0": "hemacchadanachāditā.",
  "tha-ap1:41.1_0": "Catuddisā pokkharañño,",
  "tha-ap1:41.2_0": "padumuppalasanthatā;",
  "tha-ap1:41.3_0": "Sovaṇṇarūpā khāyantu,",
  "tha-ap1:41.4_0": "padmareṇurajuggatā.",
  "tha-ap1:42.1_0": "Pupphantu pādapā sabbe,",
  "tha-ap1:42.2_0": "pāsādassa samantato;",
  "tha-ap1:42.3_0": "Sayañca pupphā muñcitvā,",
  "tha-ap1:42.4_0": "gantvā bhavanamokiruṁ.",
  "tha-ap1:43.1_0": "Sikhino tattha naccantu,",
  "tha-ap1:43.2_0": "dibbahaṁsā pakūjare;",
  "tha-ap1:43.3_0": "Karavīkā ca gāyantu,",
  "tha-ap1:43.4_0": "dijasaṅghā samantato.",
  "tha-ap1:44.1_0": "Bheriyo sabbā vajjantu,",
  "tha-ap1:44.2_0": "vīṇā sabbā rasantu tā;",
  "tha-ap1:44.3_0": "Sabbā saṅgīti vattantu,",
  "tha-ap1:44.4_0": "pāsādassa samantato.",
  "tha-ap1:45.1_0": "Yāvatā buddhakhettamhi,",
  "tha-ap1:45.2_0": "cakkavāḷe tato pare;",
  "tha-ap1:45.3_0": "Mahantā jotisampannā,",
  "tha-ap1:45.4_0": "acchinnā ratanāmayā.",
  "tha-ap1:46.1_0": "Tiṭṭhantu soṇṇapallaṅkā,",
  "tha-ap1:46.2_0": "dīparukkhā jalantu te;",
  "tha-ap1:46.3_0": "Bhavantu ekapajjotā,",
  "tha-ap1:46.4_0": "dasasahassiparamparā.",
  "tha-ap1:47.1_0": "Gaṇikā lāsikā ceva,",
  "tha-ap1:47.2_0": "naccantu accharāgaṇā;",
  "tha-ap1:47.3_0": "Nānāraṅgā padissantu,",
  "tha-ap1:47.4_0": "pāsādassa samantato.",
  "tha-ap1:48.1_0": "Dumagge pabbatagge vā,",
  "tha-ap1:48.2_0": "sinerugirimuddhani;",
  "tha-ap1:48.3_0": "Ussāpemi dhajaṁ sabbaṁ,",
  "tha-ap1:48.4_0": "vicittaṁ pañcavaṇṇikaṁ.",
  "tha-ap1:49.1_0": "Narā nāgā ca gandhabbā,",
  "tha-ap1:49.2_0": "sabbe devā upentu te;",
  "tha-ap1:49.3_0": "Namassantā pañjalikā,",
  "tha-ap1:49.4_0": "pāsādaṁ parivārayuṁ.",
  "tha-ap1:50.1_0": "Yaṁ kiñci kusalaṁ kammaṁ,",
  "tha-ap1:50.2_0": "kattabbaṁ kiriyaṁ mama;",
  "tha-ap1:50.3_0": "Kāyena vācā manasā,",
  "tha-ap1:50.4_0": "tidase sukataṁ kataṁ.",
  "tha-ap1:51.1_0": "Ye sattā saññino atthi,",
  "tha-ap1:51.2_0": "ye ca sattā asaññino;",
  "tha-ap1:51.3_0": "Kataṁ puññaphalaṁ mayhaṁ,",
  "tha-ap1:51.4_0": "sabbe bhāgī bhavantu te.",
  "tha-ap1:52.1_0": "Yesaṁ kataṁ suviditaṁ,",
  "tha-ap1:52.2_0": "dinnaṁ puññaphalaṁ mayā;",
  "tha-ap1:52.3_0": "Ye ca tattha na jānanti,",
  "tha-ap1:52.4_0": "devā gantvā nivedayuṁ.",
  "tha-ap1:53.1_0": "Sabbalokamhi ye sattā,",
  "tha-ap1:53.2_0": "Jīvantāhārahetukā;",
  "tha-ap1:53.3_0": "Manuññaṁ bhojanaṁ sabbaṁ,",
  "tha-ap1:53.4_0": "Labhantu mama cetasā.",
  "tha-ap1:54.1_0": "Manasā dānaṁ mayā dinnaṁ,",
  "tha-ap1:54.2_0": "manasā pasādamāvahiṁ;",
  "tha-ap1:54.3_0": "Pūjitā sabbasambuddhā,",
  "tha-ap1:54.4_0": "paccekā jinasāvakā.",
  "tha-ap1:55.1_0": "Tena kammena sukatena,",
  "tha-ap1:55.2_0": "cetanāpaṇidhīhi ca;",
  "tha-ap1:55.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "tha-ap1:55.4_0": "tāvatiṁsamagacchahaṁ.",
  "tha-ap1:56.1_0": "Duve bhave pajānāmi,",
  "tha-ap1:56.2_0": "devatte atha mānuse;",
  "tha-ap1:56.3_0": "Aññaṁ gatiṁ na jānāmi,",
  "tha-ap1:56.4_0": "manasā patthanāphalaṁ.",
  "tha-ap1:57.1_0": "Devānaṁ adhiko homi,",
  "tha-ap1:57.2_0": "bhavāmi manujādhipo;",
  "tha-ap1:57.3_0": "Rūpalakkhaṇasampanno,",
  "tha-ap1:57.4_0": "paññāya asamo bhave.",
  "tha-ap1:58.1_0": "Bhojanaṁ vividhaṁ seṭṭhaṁ,",
  "tha-ap1:58.2_0": "Ratanañca anappakaṁ;",
  "tha-ap1:58.3_0": "Vividhāni ca vatthāni,",
  "tha-ap1:58.4_0": "Nabhā khippaṁ upenti maṁ.",
  "tha-ap1:59.1_0": "Pathabyā pabbate ceva,",
  "tha-ap1:59.2_0": "ākāse udake vane;",
  "tha-ap1:59.3_0": "Yaṁ yaṁ hatthaṁ pasāremi,",
  "tha-ap1:59.4_0": "dibbā bhakkhā upenti maṁ.",
  "tha-ap1:60.1_0": "Pathabyā pabbate ceva,",
  "tha-ap1:60.2_0": "ākāse udake vane;",
  "tha-ap1:60.3_0": "Yaṁ yaṁ hatthaṁ pasāremi,",
  "tha-ap1:60.4_0": "ratanā sabbe upenti maṁ.",
  "tha-ap1:61.1_0": "Pathabyā pabbate ceva,",
  "tha-ap1:61.2_0": "ākāse udake vane;",
  "tha-ap1:61.3_0": "Yaṁ yaṁ hatthaṁ pasāremi,",
  "tha-ap1:61.4_0": "sabbe gandhā upenti maṁ.",
  "tha-ap1:62.1_0": "Pathabyā pabbate ceva,",
  "tha-ap1:62.2_0": "ākāse udake vane;",
  "tha-ap1:62.3_0": "Yaṁ yaṁ hatthaṁ pasāremi,",
  "tha-ap1:62.4_0": "sabbe yānā upenti maṁ.",
  "tha-ap1:63.1_0": "Pathabyā pabbate ceva,",
  "tha-ap1:63.2_0": "ākāse udake vane;",
  "tha-ap1:63.3_0": "Yaṁ yaṁ hatthaṁ pasāremi,",
  "tha-ap1:63.4_0": "sabbe mālā upenti maṁ.",
  "tha-ap1:64.1_0": "Pathabyā pabbate ceva,",
  "tha-ap1:64.2_0": "ākāse udake vane;",
  "tha-ap1:64.3_0": "Yaṁ yaṁ hatthaṁ pasāremi,",
  "tha-ap1:64.4_0": "alaṅkārā upenti maṁ.",
  "tha-ap1:65.1_0": "Pathabyā pabbate ceva,",
  "tha-ap1:65.2_0": "ākāse udake vane;",
  "tha-ap1:65.3_0": "Yaṁ yaṁ hatthaṁ pasāremi,",
  "tha-ap1:65.4_0": "sabbā kaññā upenti maṁ.",
  "tha-ap1:66.1_0": "Pathabyā pabbate ceva,",
  "tha-ap1:66.2_0": "ākāse udake vane;",
  "tha-ap1:66.3_0": "Yaṁ yaṁ hatthaṁ pasāremi,",
  "tha-ap1:66.4_0": "madhusakkharā upenti maṁ.",
  "tha-ap1:67.1_0": "Pathabyā pabbate ceva,",
  "tha-ap1:67.2_0": "ākāse udake vane;",
  "tha-ap1:67.3_0": "Yaṁ yaṁ hatthaṁ pasāremi,",
  "tha-ap1:67.4_0": "sabbe khajjā upenti maṁ.",
  "tha-ap1:68.1_0": "Adhane addhikajane,",
  "tha-ap1:68.2_0": "yācake ca pathāvino;",
  "tha-ap1:68.3_0": "Dadāmihaṁ dānavaraṁ,",
  "tha-ap1:68.4_0": "sambodhivarapattiyā.",
  "tha-ap1:69.1_0": "Nādento pabbataṁ selaṁ,",
  "tha-ap1:69.2_0": "gajjento bahalaṁ giriṁ;",
  "tha-ap1:69.3_0": "Sadevakaṁ hāsayanto,",
  "tha-ap1:69.4_0": "buddho loke bhavāmahaṁ.",
  "tha-ap1:70.1_0": "Disā dasavidhā loke,",
  "tha-ap1:70.2_0": "yāyato natthi antakaṁ;",
  "tha-ap1:70.3_0": "Tasmiñca disābhāgamhi,",
  "tha-ap1:70.4_0": "buddhakhettā asaṅkhiyā.",
  "tha-ap1:71.1_0": "Pabhā pakittitā mayhaṁ,",
  "tha-ap1:71.2_0": "yamakā raṁsivāhanā;",
  "tha-ap1:71.3_0": "Etthantare raṁsijālaṁ,",
  "tha-ap1:71.4_0": "āloko vipulo bhave.",
  "tha-ap1:72.1_0": "Ettake lokadhātumhi,",
  "tha-ap1:72.2_0": "sabbe passantu maṁ janā;",
  "tha-ap1:72.3_0": "Sabbe maṁ anuvattantu,",
  "tha-ap1:72.4_0": "yāva brahmanivesanaṁ.",
  "tha-ap1:73.1_0": "Visiṭṭhamadhunādena,",
  "tha-ap1:73.2_0": "amatabherimāhaniṁ;",
  "tha-ap1:73.3_0": "Etthantare janā sabbe,",
  "tha-ap1:73.4_0": "suṇantu madhuraṁ giraṁ.",
  "tha-ap1:74.1_0": "Dhammameghena vassante,",
  "tha-ap1:74.2_0": "sabbe hontu anāsavā;",
  "tha-ap1:74.3_0": "Yettha pacchimakā sattā,",
  "tha-ap1:74.4_0": "sotāpannā bhavantu te.",
  "tha-ap1:75.1_0": "Datvā dātabbakaṁ dānaṁ,",
  "tha-ap1:75.2_0": "sīlaṁ pūretvā asesato;",
  "tha-ap1:75.3_0": "Nekkhammapāramiṁ gantvā,",
  "tha-ap1:75.4_0": "patto sambodhimuttamaṁ.",
  "tha-ap1:76.1_0": "Paṇḍite paripucchitvā,",
  "tha-ap1:76.2_0": "katvā vīriyamuttamaṁ;",
  "tha-ap1:76.3_0": "Khantiyā pāramiṁ gantvā,",
  "tha-ap1:76.4_0": "patto sambodhimuttamaṁ.",
  "tha-ap1:77.1_0": "Katvā daḷhamadhiṭṭhānaṁ,",
  "tha-ap1:77.2_0": "saccapārami pūriya;",
  "tha-ap1:77.3_0": "Mettāya pāramiṁ gantvā,",
  "tha-ap1:77.4_0": "patto sambodhimuttamaṁ.",
  "tha-ap1:78.1_0": "Lābhālābhe sukhe dukkhe,",
  "tha-ap1:78.2_0": "sammāne cāvamānane;",
  "tha-ap1:78.3_0": "Sabbattha samako hutvā,",
  "tha-ap1:78.4_0": "patto sambodhimuttamaṁ.",
  "tha-ap1:79.1_0": "Kosajjaṁ bhayato disvā,",
  "tha-ap1:79.2_0": "vīriyañcāpi khemato;",
  "tha-ap1:79.3_0": "Āraddhavīriyā hotha,",
  "tha-ap1:79.4_0": "esā buddhānusāsanī.",
  "tha-ap1:80.1_0": "Vivādaṁ bhayato disvā,",
  "tha-ap1:80.2_0": "avivādañca khemato;",
  "tha-ap1:80.3_0": "Samaggā sakhilā hotha,",
  "tha-ap1:80.4_0": "esā buddhānusāsanī.",
  "tha-ap1:81.1_0": "Pamādaṁ bhayato disvā,",
  "tha-ap1:81.2_0": "appamādañca khemato;",
  "tha-ap1:81.3_0": "Bhāvethaṭṭhaṅgikaṁ maggaṁ,",
  "tha-ap1:81.4_0": "esā buddhānusāsanī.",
  "tha-ap1:82.1_0": "Samāgatā bahū buddhā,",
  "tha-ap1:82.2_0": "arahantā ca sabbaso;",
  "tha-ap1:82.3_0": "Sambuddhe arahante ca,",
  "tha-ap1:82.4_0": "vandamānā namassatha.",
  "tha-ap1:83.1_0": "Evaṁ acintiyā buddhā,",
  "tha-ap1:83.2_0": "buddhadhammā acintiyā;",
  "tha-ap1:83.3_0": "Acintiye pasannānaṁ,",
  "tha-ap1:83.4_0": "vipāko hoti acintiyo”.",
  "tha-ap1:84.1_0": "Itthaṁ sudaṁ bhagavā attano buddhacariyaṁ sambhāvayamāno buddhāpadāniyaṁ nāma dhammapariyāyaṁ abhāsitthāti.",
  "tha-ap1:85.1_0": "Buddhāpadānaṁ samattaṁ."
}