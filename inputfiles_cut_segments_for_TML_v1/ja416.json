{
  "ja416:0.1_0": "Jātaka",
  "ja416:0.2_0": "Sattakanipāta",
  "ja416:0.3_0": "Gandhāravagga",
  "ja416:0.4_0": "11. Parantapajātaka",
  "ja416:1.1_0": "“Āgamissati me pāpaṁ,",
  "ja416:1.2_0": "āgamissati me bhayaṁ;",
  "ja416:1.3_0": "Tadā hi calitā sākhā,",
  "ja416:1.4_0": "manussena migena vā”.",
  "ja416:2.1_0": "“Bhīruyā nūna me kāmo,",
  "ja416:2.2_0": "avidūre vasantiyā;",
  "ja416:2.3_0": "Karissati kisaṁ paṇḍuṁ,",
  "ja416:2.4_0": "sāva sākhā parantapaṁ.",
  "ja416:3.1_0": "Socayissati maṁ kantā,",
  "ja416:3.2_0": "gāme vasamaninditā;",
  "ja416:3.3_0": "Karissati kisaṁ paṇḍuṁ,",
  "ja416:3.4_0": "sāva sākhā parantapaṁ.",
  "ja416:4.1_0": "Tayā maṁ asitāpaṅgi,",
  "ja416:4.2_0": "sitāni bhaṇitāni ca;",
  "ja416:4.3_0": "Kisaṁ paṇḍuṁ karissanti,",
  "ja416:4.4_0": "sāva sākhā parantapaṁ”.",
  "ja416:5.1_0": "“Agamā nūna so saddo,",
  "ja416:5.2_0": "asaṁsi nūna so tava;",
  "ja416:5.3_0": "Akkhātaṁ nūna taṁ tena,",
  "ja416:5.4_0": "yo taṁ sākhamakampayi.",
  "ja416:6.1_0": "Idaṁ kho taṁ samāgamma,",
  "ja416:6.2_0": "mama bālassa cintitaṁ;",
  "ja416:6.3_0": "Tadā hi calitā sākhā,",
  "ja416:6.4_0": "manussena migena vā”.",
  "ja416:7.1_0": "“Tatheva tvaṁ avedesi,",
  "ja416:7.2_0": "Avañci pitaraṁ mama;",
  "ja416:7.3_0": "Hantvā sākhāhi chādento,",
  "ja416:7.4_0": "Āgamissati me bhayan”ti. ",
  "ja416:8.1_0": "Parantapajātakaṁ ekādasamaṁ.",
  "ja416:8.2_0": "Gandhāravaggo dutiyo.",
  "ja416:9.1_0": "Tassuddānaṁ",
  "ja416:10.1_0": "Varagāma mahākapi bhaggava ca,",
  "ja416:10.2_0": "Daḷhadhamma sakuñjara kesavaro;",
  "ja416:10.3_0": "Urago vidhuro puna jāgarataṁ,",
  "ja416:10.4_0": "Atha kosalādhipa parantapa cāti.",
  "ja416:11.1_0": "Atha vagguddānaṁ",
  "ja416:12.1_0": "Atha sattanipātamhi,",
  "ja416:12.2_0": "vaggaṁ me bhaṇato suṇa;",
  "ja416:12.3_0": "Kukku ca puna gandhāro,",
  "ja416:12.4_0": "dveva vuttā mahesināti.",
  "ja416:13.1_0": "Sattakanipātaṁ niṭṭhitaṁ."
}