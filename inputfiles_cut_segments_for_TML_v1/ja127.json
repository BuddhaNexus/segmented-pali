{
  "ja127:0.1_0": "Jātaka",
  "ja127:0.2_0": "Ekakanipāta",
  "ja127:0.3_0": "Kusanāḷivagga",
  "ja127:0.4_0": "7. Kalaṇḍukajātaka",
  "ja127:1.1_0": "“Te desā tāni vatthūni,",
  "ja127:1.2_0": "ahañca vanagocaro;",
  "ja127:1.3_0": "Anuvicca kho taṁ gaṇheyyuṁ,",
  "ja127:1.4_0": "piva khīraṁ kalaṇḍukā”ti.",
  "ja127:2.1_0": "Kalaṇḍukajātakaṁ sattamaṁ."
}