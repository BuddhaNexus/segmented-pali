{
  "ja223:0.1_0": "Jātaka",
  "ja223:0.2_0": "Dukanipāta",
  "ja223:0.3_0": "Kāsāvavagga",
  "ja223:0.4_0": "3. Puṭabhattajātaka",
  "ja223:1.1_0": "“Name namantassa bhaje bhajantaṁ,",
  "ja223:1.2_0": "Kiccānukubbassa kareyya kiccaṁ;",
  "ja223:1.3_0": "Nānatthakāmassa kareyya atthaṁ,",
  "ja223:1.4_0": "Asambhajantampi na sambhajeyya.",
  "ja223:2.1_0": "Caje cajantaṁ vanathaṁ na kayirā,",
  "ja223:2.2_0": "Apetacittena na sambhajeyya;",
  "ja223:2.3_0": "Dijo dumaṁ khīṇaphalanti ñatvā,",
  "ja223:2.4_0": "Aññaṁ samekkheyya mahā hi loko”ti.",
  "ja223:3.1_0": "Puṭabhattajātakaṁ tatiyaṁ."
}