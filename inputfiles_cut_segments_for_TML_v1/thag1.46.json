{
  "thag1.46:0.1_0": "Theragāthā",
  "thag1.46:0.2_0": "Ekakanipāta",
  "thag1.46:0.3_0": "Pañcamavagga",
  "thag1.46:0.4_0": "1.46. Samiddhittheragāthā",
  "thag1.46:1.1_0": "“Saddhāyāhaṁ pabbajito,",
  "thag1.46:1.2_0": "Agārasmānagāriyaṁ;",
  "thag1.46:1.3_0": "Sati paññā ca me vuḍḍhā,",
  "thag1.46:1.4_0": "Cittañca susamāhitaṁ;",
  "thag1.46:1.5_0": "Kāmaṁ karassu rūpāni,",
  "thag1.46:1.6_0": "Neva maṁ byādhayissasī”ti.",
  "thag1.46:1.7_0": "… Samiddhitthero …."
}