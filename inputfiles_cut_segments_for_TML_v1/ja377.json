{
  "ja377:0.1_0": "Jātaka",
  "ja377:0.2_0": "Chakkanipāta",
  "ja377:0.3_0": "Avāriyavagga",
  "ja377:0.4_0": "2. Setaketujātaka",
  "ja377:1.1_0": "“Mā tāta kujjhi na hi sādhu kodho,",
  "ja377:1.2_0": "Bahumpi te adiṭṭhamassutañca;",
  "ja377:1.3_0": "Mātā pitā disatā setaketu,",
  "ja377:1.4_0": "Ācariyamāhu disataṁ pasatthā.",
  "ja377:2.1_0": "Agārino annadapānavatthadā,",
  "ja377:2.2_0": "Avhāyikā tampi disaṁ vadanti;",
  "ja377:2.3_0": "Esā disā paramā setaketu,",
  "ja377:2.4_0": "Yaṁ patvā dukkhī sukhino bhavanti”.",
  "ja377:3.1_0": "“Kharājinā jaṭilā paṅkadantā,",
  "ja377:3.2_0": "Dummakkharūpā yeme jappanti mante;",
  "ja377:3.3_0": "Kacci nu te mānusake payoge,",
  "ja377:3.4_0": "Idaṁ vidū parimuttā apāyā”.",
  "ja377:4.1_0": "“Pāpāni kammāni katvāna rāja,",
  "ja377:4.2_0": "Bahussuto ce na careyya dhammaṁ;",
  "ja377:4.3_0": "Sahassavedopi na taṁ paṭicca,",
  "ja377:4.4_0": "Dukkhā pamucce caraṇaṁ apatvā”.",
  "ja377:5.1_0": "“Sahassavedopi na taṁ paṭicca,",
  "ja377:5.2_0": "Dukkhā pamucce caraṇaṁ apatvā;",
  "ja377:5.3_0": "Maññāmi vedā aphalā bhavanti,",
  "ja377:5.4_0": "Sasaṁyamaṁ caraṇameva saccaṁ”.",
  "ja377:6.1_0": "“Na heva vedā aphalā bhavanti,",
  "ja377:6.2_0": "Sasaṁyamaṁ caraṇameva saccaṁ;",
  "ja377:6.3_0": "Kittiñhi pappoti adhicca vede,",
  "ja377:6.4_0": "Santiṁ puṇeti caraṇena danto”ti.",
  "ja377:7.1_0": "Setaketujātakaṁ dutiyaṁ."
}