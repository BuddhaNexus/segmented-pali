{
  "thi-ap17:0.1_0": "Therīapadāna",
  "thi-ap17:0.2_0": "Ekūposathikavagga",
  "thi-ap17:0.3_0": "7. Mahāpajāpatigotamītherīapadāna",
  "thi-ap17:1.1_0": "Ekadā lokapajjoto,",
  "thi-ap17:1.2_0": "vesāliyaṁ mahāvane;",
  "thi-ap17:1.3_0": "Kūṭāgāre susālāyaṁ,",
  "thi-ap17:1.4_0": "vasate narasārathi.",
  "thi-ap17:2.1_0": "Tadā jinassa mātucchā,",
  "thi-ap17:2.2_0": "mahāgotami bhikkhunī;",
  "thi-ap17:2.3_0": "Tahiṁ kate pure ramme,",
  "thi-ap17:2.4_0": "vasī bhikkhunupassaye.",
  "thi-ap17:3.1_0": "Bhikkhunīhi vimuttāhi,",
  "thi-ap17:3.2_0": "satehi saha pañcahi;",
  "thi-ap17:3.3_0": "Rahogatāya tassevaṁ,",
  "thi-ap17:3.4_0": "citassāsi vitakkitaṁ.",
  "thi-ap17:4.1_0": "“Buddhassa parinibbānaṁ,",
  "thi-ap17:4.2_0": "sāvakaggayugassa vā;",
  "thi-ap17:4.3_0": "Rāhulānandanandānaṁ,",
  "thi-ap17:4.4_0": "nāhaṁ lacchāmi passituṁ.",
  "thi-ap17:5.1_0": "Buddhassa parinibbānā,",
  "thi-ap17:5.2_0": "sāvakaggayugassa vā;",
  "thi-ap17:5.3_0": "Mahākassapanandānaṁ,",
  "thi-ap17:5.4_0": "ānandarāhulāna ca.",
  "thi-ap17:6.1_0": "Paṭikaccāyusaṅkhāraṁ,",
  "thi-ap17:6.2_0": "osajjitvāna nibbutiṁ;",
  "thi-ap17:6.3_0": "Gaccheyyaṁ lokanāthena,",
  "thi-ap17:6.4_0": "anuññātā mahesinā”.",
  "thi-ap17:7.1_0": "Tathā pañcasatānampi,",
  "thi-ap17:7.2_0": "bhikkhunīnaṁ vitakkitaṁ;",
  "thi-ap17:7.3_0": "Āsi khemādikānampi,",
  "thi-ap17:7.4_0": "etadeva vitakkitaṁ.",
  "thi-ap17:8.1_0": "Bhūmicālo tadā āsi,",
  "thi-ap17:8.2_0": "nāditā devadundubhī;",
  "thi-ap17:8.3_0": "Upassayādhivatthāyo,",
  "thi-ap17:8.4_0": "devatā sokapīḷitā.",
  "thi-ap17:9.1_0": "Vilapantā sukaruṇaṁ,",
  "thi-ap17:9.2_0": "tatthassūni pavattayuṁ;",
  "thi-ap17:9.3_0": "Mittā bhikkhuniyo tāhi,",
  "thi-ap17:9.4_0": "upagantvāna gotamiṁ.",
  "thi-ap17:10.1_0": "Nipacca sirasā pāde,",
  "thi-ap17:10.2_0": "idaṁ vacanamabravuṁ;",
  "thi-ap17:10.3_0": "Tattha toyalavāsittā,",
  "thi-ap17:10.4_0": "mayamayye rahogatā.",
  "thi-ap17:11.1_0": "Sā calā calitā bhūmi,",
  "thi-ap17:11.2_0": "nāditā devadundubhī;",
  "thi-ap17:11.3_0": "Paridevā ca suyyante,",
  "thi-ap17:11.4_0": "kimatthaṁ nūna gotamī.",
  "thi-ap17:12.1_0": "Tadā avoca sā sabbaṁ,",
  "thi-ap17:12.2_0": "yathāparivitakkitaṁ;",
  "thi-ap17:12.3_0": "Tāyopi sabbā āhaṁsu,",
  "thi-ap17:12.4_0": "yathāparivitakkitaṁ.",
  "thi-ap17:13.1_0": "“Yadi te rucitaṁ ayye,",
  "thi-ap17:13.2_0": "nibbānaṁ paramaṁ sivaṁ;",
  "thi-ap17:13.3_0": "Nibbāyissāma sabbāpi,",
  "thi-ap17:13.4_0": "buddhānuññāya subbate.",
  "thi-ap17:14.1_0": "Mayaṁ sahāva nikkhantā,",
  "thi-ap17:14.2_0": "gharāpi ca bhavāpi ca;",
  "thi-ap17:14.3_0": "Sahāyeva gamissāma,",
  "thi-ap17:14.4_0": "nibbānaṁ padamuttamaṁ”.",
  "thi-ap17:15.1_0": "“Nibbānāya vajantīnaṁ,",
  "thi-ap17:15.2_0": "kiṁ vakkhāmī”ti sā vadaṁ;",
  "thi-ap17:15.3_0": "Saha sabbāhi niggañchi,",
  "thi-ap17:15.4_0": "bhikkhunīnilayā tadā.",
  "thi-ap17:16.1_0": "“Upassaye yādhivatthā,",
  "thi-ap17:16.2_0": "devatā tā khamantu me;",
  "thi-ap17:16.3_0": "Bhikkhunīnilayassedaṁ,",
  "thi-ap17:16.4_0": "pacchimaṁ dassanaṁ mama.",
  "thi-ap17:17.1_0": "Na jarā maccu vā yattha,",
  "thi-ap17:17.2_0": "appiyehi samāgamo;",
  "thi-ap17:17.3_0": "Piyehi na viyogotthi,",
  "thi-ap17:17.4_0": "taṁ vajissaṁ asaṅkhataṁ”.",
  "thi-ap17:18.1_0": "Avītarāgā taṁ sutvā,",
  "thi-ap17:18.2_0": "vacanaṁ sugatorasā;",
  "thi-ap17:18.3_0": "Sokaṭṭā parideviṁsu,",
  "thi-ap17:18.4_0": "“aho no appapuññatā.",
  "thi-ap17:19.1_0": "Bhikkhunīnilayo suñño,",
  "thi-ap17:19.2_0": "bhūto tāhi vinā ayaṁ;",
  "thi-ap17:19.3_0": "Pabhāte viya tārāyo,",
  "thi-ap17:19.4_0": "na dissanti jinorasā.",
  "thi-ap17:20.1_0": "Nibbānaṁ gotamī yāti,",
  "thi-ap17:20.2_0": "satehi saha pañcahi;",
  "thi-ap17:20.3_0": "Nadīsatehiva saha,",
  "thi-ap17:20.4_0": "gaṅgā pañcahi sāgaraṁ”.",
  "thi-ap17:21.1_0": "Rathiyāya vajantiyo,",
  "thi-ap17:21.2_0": "disvā saddhā upāsikā;",
  "thi-ap17:21.3_0": "Gharā nikkhamma pādesu,",
  "thi-ap17:21.4_0": "nipacca idamabravuṁ.",
  "thi-ap17:22.1_0": "“Pasīdassu mahābhoge,",
  "thi-ap17:22.2_0": "anāthāyo vihāya no;",
  "thi-ap17:22.3_0": "Tayā na yuttā nibbātuṁ,",
  "thi-ap17:22.4_0": "icchaṭṭā vilapiṁsu tā”.",
  "thi-ap17:23.1_0": "Tāsaṁ sokapahānatthaṁ,",
  "thi-ap17:23.2_0": "avoca madhuraṁ giraṁ;",
  "thi-ap17:23.3_0": "“Ruditena alaṁ puttā,",
  "thi-ap17:23.4_0": "hāsakāloyamajja vo.",
  "thi-ap17:24.1_0": "Pariññātaṁ mayā dukkhaṁ,",
  "thi-ap17:24.2_0": "dukkhahetu vivajjito;",
  "thi-ap17:24.3_0": "Nirodho me sacchikato,",
  "thi-ap17:24.4_0": "maggo cāpi subhāvito.",
  "thi-ap17:25.1_0": "Paṭhamaṁ bhāṇavāraṁ.",
  "thi-ap17:26.1_0": "Pariciṇṇo mayā satthā,",
  "thi-ap17:26.2_0": "kataṁ buddhassa sāsanaṁ;",
  "thi-ap17:26.3_0": "Ohito garuko bhāro,",
  "thi-ap17:26.4_0": "bhavanetti samūhatā.",
  "thi-ap17:27.1_0": "Yassatthāya pabbajitā,",
  "thi-ap17:27.2_0": "agārasmānagāriyaṁ;",
  "thi-ap17:27.3_0": "So me attho anuppatto,",
  "thi-ap17:27.4_0": "sabbasaṁyojanakkhayo.",
  "thi-ap17:28.1_0": "Buddho tassa ca saddhammo,",
  "thi-ap17:28.2_0": "anūno yāva tiṭṭhati;",
  "thi-ap17:28.3_0": "Nibbātuṁ tāva kālo me,",
  "thi-ap17:28.4_0": "mā maṁ socatha puttikā.",
  "thi-ap17:29.1_0": "Koṇḍaññānandanandādī,",
  "thi-ap17:29.2_0": "tiṭṭhanti rāhulo jino;",
  "thi-ap17:29.3_0": "Sukhito sahito saṅgho,",
  "thi-ap17:29.4_0": "hatadabbā ca titthiyā.",
  "thi-ap17:30.1_0": "Okkākavaṁsassa yaso,",
  "thi-ap17:30.2_0": "ussito māramaddano;",
  "thi-ap17:30.3_0": "Nanu sampati kālo me,",
  "thi-ap17:30.4_0": "nibbānatthāya puttikā.",
  "thi-ap17:31.1_0": "Cirappabhuti yaṁ mayhaṁ,",
  "thi-ap17:31.2_0": "patthitaṁ ajja sijjhate;",
  "thi-ap17:31.3_0": "Ānandabherikāloyaṁ,",
  "thi-ap17:31.4_0": "kiṁ vo assūhi puttikā.",
  "thi-ap17:32.1_0": "Sace mayi dayā atthi,",
  "thi-ap17:32.2_0": "yadi catthi kataññutā;",
  "thi-ap17:32.3_0": "Saddhammaṭṭhitiyā sabbā,",
  "thi-ap17:32.4_0": "karotha vīriyaṁ daḷhaṁ.",
  "thi-ap17:33.1_0": "Thīnaṁ adāsi pabbajjaṁ,",
  "thi-ap17:33.2_0": "sambuddho yācito mayā;",
  "thi-ap17:33.3_0": "Tasmā yathāhaṁ nandissaṁ,",
  "thi-ap17:33.4_0": "tathā tamanutiṭṭhatha”.",
  "thi-ap17:34.1_0": "Tā evamanusāsitvā,",
  "thi-ap17:34.2_0": "bhikkhunīhi purakkhatā;",
  "thi-ap17:34.3_0": "Upecca buddhaṁ vanditvā,",
  "thi-ap17:34.4_0": "idaṁ vacanamabravi.",
  "thi-ap17:35.1_0": "“Ahaṁ sugata te mātā,",
  "thi-ap17:35.2_0": "tvañca vīra pitā mama;",
  "thi-ap17:35.3_0": "Saddhammasukhada nātha,",
  "thi-ap17:35.4_0": "tayā jātāmhi gotama.",
  "thi-ap17:36.1_0": "Saṁvaddhitoyaṁ sugata,",
  "thi-ap17:36.2_0": "rūpakāyo mayā tava;",
  "thi-ap17:36.3_0": "Anindito dhammakāyo,",
  "thi-ap17:36.4_0": "mama saṁvaddhito tayā.",
  "thi-ap17:37.1_0": "Muhuttaṁ taṇhāsamaṇaṁ,",
  "thi-ap17:37.2_0": "khīraṁ tvaṁ pāyito mayā;",
  "thi-ap17:37.3_0": "Tayāhaṁ santamaccantaṁ,",
  "thi-ap17:37.4_0": "dhammakhīrañhi pāyitā.",
  "thi-ap17:38.1_0": "Bandhanārakkhane mayhaṁ,",
  "thi-ap17:38.2_0": "anaṇo tvaṁ mahāmune;",
  "thi-ap17:38.3_0": "Puttakāmā thiyo yācaṁ,",
  "thi-ap17:38.4_0": "labhanti tādisaṁ sutaṁ.",
  "thi-ap17:39.1_0": "Mandhātādinarindānaṁ,",
  "thi-ap17:39.2_0": "yā mātā sā bhavaṇṇave;",
  "thi-ap17:39.3_0": "Nimuggāhaṁ tayā putta,",
  "thi-ap17:39.4_0": "tāritā bhavasāgarā.",
  "thi-ap17:40.1_0": "Rañño mātā mahesīti,",
  "thi-ap17:40.2_0": "sulabhaṁ nāmamitthinaṁ;",
  "thi-ap17:40.3_0": "Buddhamātāti yaṁ nāmaṁ,",
  "thi-ap17:40.4_0": "etaṁ paramadullabhaṁ.",
  "thi-ap17:41.1_0": "Tañca laddhaṁ mahāvīra,",
  "thi-ap17:41.2_0": "paṇidhānaṁ mamaṁ tayā;",
  "thi-ap17:41.3_0": "Aṇukaṁ vā mahantaṁ vā,",
  "thi-ap17:41.4_0": "taṁ sabbaṁ pūritaṁ mayā.",
  "thi-ap17:42.1_0": "Parinibbātumicchāmi,",
  "thi-ap17:42.2_0": "vihāyemaṁ kaḷevaraṁ;",
  "thi-ap17:42.3_0": "Anujānāhi me vīra,",
  "thi-ap17:42.4_0": "dukkhantakara nāyaka.",
  "thi-ap17:43.1_0": "Cakkaṅkusadhajākiṇṇe,",
  "thi-ap17:43.2_0": "pāde kamalakomale;",
  "thi-ap17:43.3_0": "Pasārehi paṇāmaṁ te,",
  "thi-ap17:43.4_0": "karissaṁ puttauttame.",
  "thi-ap17:44.1_0": "Suvaṇṇarāsisaṅkāsaṁ,",
  "thi-ap17:44.2_0": "sarīraṁ kuru pākaṭaṁ;",
  "thi-ap17:44.3_0": "Katvā dehaṁ sudiṭṭhaṁ te,",
  "thi-ap17:44.4_0": "santiṁ gacchāmi nāyaka”.",
  "thi-ap17:45.1_0": "Dvattiṁsalakkhaṇūpetaṁ,",
  "thi-ap17:45.2_0": "suppabhālaṅkataṁ tanuṁ;",
  "thi-ap17:45.3_0": "Sañjhāghanāva bālakkaṁ,",
  "thi-ap17:45.4_0": "mātucchaṁ dassayī jino.",
  "thi-ap17:46.1_0": "Phullāravindasaṅkāse,",
  "thi-ap17:46.2_0": "taruṇādiccasappabhe;",
  "thi-ap17:46.3_0": "Cakkaṅkite pādatale,",
  "thi-ap17:46.4_0": "tato sā sirasā pati.",
  "thi-ap17:47.1_0": "“Paṇamāmi narādicca,",
  "thi-ap17:47.2_0": "ādiccakulaketukaṁ;",
  "thi-ap17:47.3_0": "Pacchime maraṇe mayhaṁ,",
  "thi-ap17:47.4_0": "na taṁ ikkhāmahaṁ puno.",
  "thi-ap17:48.1_0": "Itthiyo nāma lokagga,",
  "thi-ap17:48.2_0": "sabbadosākarā matā;",
  "thi-ap17:48.3_0": "Yadi ko catthi doso me,",
  "thi-ap17:48.4_0": "khamassu karuṇākara.",
  "thi-ap17:49.1_0": "Itthikānañca pabbajjaṁ,",
  "thi-ap17:49.2_0": "haṁ taṁ yāciṁ punappunaṁ;",
  "thi-ap17:49.3_0": "Tattha ce atthi doso me,",
  "thi-ap17:49.4_0": "taṁ khamassu narāsabha.",
  "thi-ap17:50.1_0": "Mayā bhikkhuniyo vīra,",
  "thi-ap17:50.2_0": "tavānuññāya sāsitā;",
  "thi-ap17:50.3_0": "Tatra ce atthi dunnītaṁ,",
  "thi-ap17:50.4_0": "taṁ khamassu khamādhipa”.",
  "thi-ap17:51.1_0": "“Akkhante nāma khantabbaṁ,",
  "thi-ap17:51.2_0": "kiṁ bhave guṇabhūsane;",
  "thi-ap17:51.3_0": "Kimuttaraṁ te vakkhāmi,",
  "thi-ap17:51.4_0": "nibbānāya vajantiyā.",
  "thi-ap17:52.1_0": "Suddhe anūne mama bhikkhusaṅghe,",
  "thi-ap17:52.2_0": "Lokā ito nissarituṁ khamante;",
  "thi-ap17:52.3_0": "Pabhātakāle byasanaṅgatānaṁ,",
  "thi-ap17:52.4_0": "Disvāna niyyātiva candalekhā”.",
  "thi-ap17:53.1_0": "Tadetarā bhikkhuniyo jinaggaṁ,",
  "thi-ap17:53.2_0": "Tārāva candānugatā sumeruṁ;",
  "thi-ap17:53.3_0": "Padakkhiṇaṁ kacca nipacca pāde,",
  "thi-ap17:53.4_0": "Ṭhitā mukhantaṁ samudikkhamānā.",
  "thi-ap17:54.1_0": "“Na tittipubbaṁ tava dassanena,",
  "thi-ap17:54.2_0": "Cakkhuṁ na sotaṁ tava bhāsitena;",
  "thi-ap17:54.3_0": "Cittaṁ mamaṁ kevalamekameva,",
  "thi-ap17:54.4_0": "Pappuyya taṁ dhammarasena titti.",
  "thi-ap17:55.1_0": "Nadato parisāyaṁ te,",
  "thi-ap17:55.2_0": "vāditabbapahārino;",
  "thi-ap17:55.3_0": "Ye te dakkhanti vadanaṁ,",
  "thi-ap17:55.4_0": "dhaññā te narapuṅgava.",
  "thi-ap17:56.1_0": "Dīghaṅgulī tambanakhe,",
  "thi-ap17:56.2_0": "subhe āyatapaṇhike;",
  "thi-ap17:56.3_0": "Ye pāde paṇamissanti,",
  "thi-ap17:56.4_0": "tepi dhaññā guṇandhara.",
  "thi-ap17:57.1_0": "Madhurāni pahaṭṭhāni,",
  "thi-ap17:57.2_0": "dosagghāni hitāni ca;",
  "thi-ap17:57.3_0": "Ye te vākyāni suyyanti,",
  "thi-ap17:57.4_0": "tepi dhaññā naruttama.",
  "thi-ap17:58.1_0": "Dhaññāhaṁ te mahāvīra,",
  "thi-ap17:58.2_0": "pādapūjanatapparā;",
  "thi-ap17:58.3_0": "Tiṇṇasaṁsārakantārā,",
  "thi-ap17:58.4_0": "suvākyena sirīmato”.",
  "thi-ap17:59.1_0": "Tato sā anusāvetvā,",
  "thi-ap17:59.2_0": "bhikkhusaṅghampi subbatā;",
  "thi-ap17:59.3_0": "Rāhulānandanande ca,",
  "thi-ap17:59.4_0": "vanditvā idamabravi.",
  "thi-ap17:60.1_0": "“Āsīvisālayasame,",
  "thi-ap17:60.2_0": "rogāvāse kaḷevare;",
  "thi-ap17:60.3_0": "Nibbindā dukkhasaṅghāṭe,",
  "thi-ap17:60.4_0": "jarāmaraṇagocare.",
  "thi-ap17:61.1_0": "Nānākalimalākiṇṇe,",
  "thi-ap17:61.2_0": "parāyatte nirīhake;",
  "thi-ap17:61.3_0": "Tena nibbātumicchāmi,",
  "thi-ap17:61.4_0": "anumaññatha puttakā”.",
  "thi-ap17:62.1_0": "Nando rāhulabhaddo ca,",
  "thi-ap17:62.2_0": "vītasokā nirāsavā;",
  "thi-ap17:62.3_0": "Ṭhitācalaṭṭhiti thirā,",
  "thi-ap17:62.4_0": "dhammatamanucintayuṁ.",
  "thi-ap17:63.1_0": "“Dhiratthu saṅkhataṁ lolaṁ,",
  "thi-ap17:63.2_0": "asāraṁ kadalūpamaṁ;",
  "thi-ap17:63.3_0": "Māyāmarīcisadisaṁ,",
  "thi-ap17:63.4_0": "ittaraṁ anavaṭṭhitaṁ.",
  "thi-ap17:64.1_0": "Yattha nāma jinassāyaṁ,",
  "thi-ap17:64.2_0": "mātucchā buddhaposikā;",
  "thi-ap17:64.3_0": "Gotamī nidhanaṁ yāti,",
  "thi-ap17:64.4_0": "aniccaṁ sabbasaṅkhataṁ”.",
  "thi-ap17:65.1_0": "Ānando ca tadā sekho,",
  "thi-ap17:65.2_0": "sokaṭṭo jinavacchalo;",
  "thi-ap17:65.3_0": "Tatthassūni karonto so,",
  "thi-ap17:65.4_0": "karuṇaṁ paridevati.",
  "thi-ap17:66.1_0": "“Hā santiṁ gotamī yāti,",
  "thi-ap17:66.2_0": "nūna buddhopi nibbutiṁ;",
  "thi-ap17:66.3_0": "Gacchati na cireneva,",
  "thi-ap17:66.4_0": "aggiriva nirindhano”.",
  "thi-ap17:67.1_0": "Evaṁ vilāpamānaṁ taṁ,",
  "thi-ap17:67.2_0": "ānandaṁ āha gotamī;",
  "thi-ap17:67.3_0": "Sutasāgaragambhīra-",
  "thi-ap17:67.4_0": "buddhopaṭṭhānatappara.",
  "thi-ap17:68.1_0": "“Na yuttaṁ socituṁ putta,",
  "thi-ap17:68.2_0": "hāsakāle upaṭṭhite;",
  "thi-ap17:68.3_0": "Tayā me saraṇaṁ putta,",
  "thi-ap17:68.4_0": "nibbānaṁ tamupāgataṁ.",
  "thi-ap17:69.1_0": "Tayā tāta samajjhiṭṭho,",
  "thi-ap17:69.2_0": "pabbajjaṁ anujāni no;",
  "thi-ap17:69.3_0": "Mā putta vimano hohi,",
  "thi-ap17:69.4_0": "saphalo te parissamo.",
  "thi-ap17:70.1_0": "Yaṁ na diṭṭhaṁ purāṇehi,",
  "thi-ap17:70.2_0": "titthikācariyehipi;",
  "thi-ap17:70.3_0": "Taṁ padaṁ sukumārīhi,",
  "thi-ap17:70.4_0": "sattavassāhi veditaṁ.",
  "thi-ap17:71.1_0": "Buddhasāsanapāleta,",
  "thi-ap17:71.2_0": "pacchimaṁ dassanaṁ tava;",
  "thi-ap17:71.3_0": "Tattha gacchāmahaṁ putta,",
  "thi-ap17:71.4_0": "gato yattha na dissate.",
  "thi-ap17:72.1_0": "Kadāci dhammaṁ desento,",
  "thi-ap17:72.2_0": "khipī lokagganāyako;",
  "thi-ap17:72.3_0": "Tadāhaṁ āsīsavācaṁ,",
  "thi-ap17:72.4_0": "avocaṁ anukampikā.",
  "thi-ap17:73.1_0": "‘Ciraṁ jīva mahāvīra,",
  "thi-ap17:73.2_0": "kappaṁ tiṭṭha mahāmune;",
  "thi-ap17:73.3_0": "Sabbalokassa atthāya,",
  "thi-ap17:73.4_0": "bhavassu ajarāmaro’.",
  "thi-ap17:74.1_0": "Taṁ tathāvādiniṁ buddho,",
  "thi-ap17:74.2_0": "mamaṁ so etadabravi;",
  "thi-ap17:74.3_0": "‘Na hevaṁ vandiyā buddhā,",
  "thi-ap17:74.4_0": "yathā vandasi gotamī’.",
  "thi-ap17:75.1_0": "‘Kathaṁ carahi sabbaññū,",
  "thi-ap17:75.2_0": "vanditabbā tathāgatā;",
  "thi-ap17:75.3_0": "Kathaṁ avandiyā buddhā,",
  "thi-ap17:75.4_0": "taṁ me akkhāhi pucchito’.",
  "thi-ap17:76.1_0": "‘Āraddhavīriye pahitatte,",
  "thi-ap17:76.2_0": "Niccaṁ daḷhaparakkame;",
  "thi-ap17:76.3_0": "Samagge sāvake passa,",
  "thi-ap17:76.4_0": "Etaṁ buddhānavandanaṁ’.",
  "thi-ap17:77.1_0": "Tato upassayaṁ gantvā,",
  "thi-ap17:77.2_0": "ekikāhaṁ vicintayiṁ;",
  "thi-ap17:77.3_0": "‘Samaggaparisaṁ nātho,",
  "thi-ap17:77.4_0": "rodhesi tibhavantago.",
  "thi-ap17:78.1_0": "Handāhaṁ parinibbissaṁ,",
  "thi-ap17:78.2_0": "mā vipattitamaddasaṁ’;",
  "thi-ap17:78.3_0": "Evāhaṁ cintayitvāna,",
  "thi-ap17:78.4_0": "disvāna isisattamaṁ.",
  "thi-ap17:79.1_0": "Parinibbānakālaṁ me,",
  "thi-ap17:79.2_0": "ārocesiṁ vināyakaṁ;",
  "thi-ap17:79.3_0": "Tato so samanuññāsi,",
  "thi-ap17:79.4_0": "‘kālaṁ jānāhi gotamī’.",
  "thi-ap17:80.1_0": "Kilesā jhāpitā mayhaṁ,",
  "thi-ap17:80.2_0": "…pe…",
  "thi-ap17:80.3_0": "viharāmi anāsavā.",
  "thi-ap17:81.1_0": "Svāgataṁ vata me āsi,",
  "thi-ap17:81.2_0": "…pe…",
  "thi-ap17:81.3_0": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap17:82.1_0": "Paṭisambhidā catasso,",
  "thi-ap17:82.2_0": "…pe…",
  "thi-ap17:82.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "thi-ap17:83.1_0": "“Thīnaṁ dhammābhisamaye,",
  "thi-ap17:83.2_0": "ye bālā vimatiṁ gatā;",
  "thi-ap17:83.3_0": "Tesaṁ diṭṭhippahānatthaṁ,",
  "thi-ap17:83.4_0": "iddhiṁ dassehi gotamī”.",
  "thi-ap17:84.1_0": "Tadā nipacca sambuddhaṁ,",
  "thi-ap17:84.2_0": "uppatitvāna ambaraṁ;",
  "thi-ap17:84.3_0": "Iddhī anekā dassesi,",
  "thi-ap17:84.4_0": "buddhānuññāya gotamī.",
  "thi-ap17:85.1_0": "Ekikā bahudhā āsi,",
  "thi-ap17:85.2_0": "bahukā cekikā tathā;",
  "thi-ap17:85.3_0": "Āvibhāvaṁ tirobhāvaṁ,",
  "thi-ap17:85.4_0": "tirokuṭṭaṁ tironagaṁ.",
  "thi-ap17:86.1_0": "Asajjamānā agamā,",
  "thi-ap17:86.2_0": "bhūmiyampi nimujjatha;",
  "thi-ap17:86.3_0": "Abhijjamāne udake,",
  "thi-ap17:86.4_0": "agañchi mahiyā yathā.",
  "thi-ap17:87.1_0": "Sakuṇīva tathākāse,",
  "thi-ap17:87.2_0": "pallaṅkena kamī tadā;",
  "thi-ap17:87.3_0": "Vasaṁ vattesi kāyena,",
  "thi-ap17:87.4_0": "yāva brahmanivesanaṁ.",
  "thi-ap17:88.1_0": "Sineruṁ daṇḍaṁ katvāna,",
  "thi-ap17:88.2_0": "chattaṁ katvā mahāmahiṁ;",
  "thi-ap17:88.3_0": "Samūlaṁ parivattetvā,",
  "thi-ap17:88.4_0": "dhārayaṁ caṅkamī nabhe.",
  "thi-ap17:89.1_0": "Chassūrodayakāleva,",
  "thi-ap17:89.2_0": "lokañcākāsi dhūmikaṁ;",
  "thi-ap17:89.3_0": "Yugante viya lokaṁ sā,",
  "thi-ap17:89.4_0": "jālāmālākulaṁ akā.",
  "thi-ap17:90.1_0": "Mucalindaṁ mahāselaṁ,",
  "thi-ap17:90.2_0": "merumūlanadantare;",
  "thi-ap17:90.3_0": "Sāsapāriva sabbāni,",
  "thi-ap17:90.4_0": "ekenaggahi muṭṭhinā.",
  "thi-ap17:91.1_0": "Aṅgulaggena chādesi,",
  "thi-ap17:91.2_0": "bhākaraṁ sanisākaraṁ;",
  "thi-ap17:91.3_0": "Candasūrasahassāni,",
  "thi-ap17:91.4_0": "āveḷamiva dhārayi.",
  "thi-ap17:92.1_0": "Catusāgaratoyāni,",
  "thi-ap17:92.2_0": "dhārayī ekapāṇinā;",
  "thi-ap17:92.3_0": "Yugantajaladākāraṁ,",
  "thi-ap17:92.4_0": "mahāvassaṁ pavassatha.",
  "thi-ap17:93.1_0": "Cakkavattiṁ saparisaṁ,",
  "thi-ap17:93.2_0": "māpayī sā nabhattale;",
  "thi-ap17:93.3_0": "Garuḷaṁ dviradaṁ sīhaṁ,",
  "thi-ap17:93.4_0": "vinadantaṁ padassayi.",
  "thi-ap17:94.1_0": "Ekikā abhinimmitvā,",
  "thi-ap17:94.2_0": "Appameyyaṁ bhikkhunīgaṇaṁ;",
  "thi-ap17:94.3_0": "Puna antaradhāpetvā,",
  "thi-ap17:94.4_0": "Ekikā munimabravi.",
  "thi-ap17:95.1_0": "“Mātucchā te mahāvīra,",
  "thi-ap17:95.2_0": "tava sāsanakārikā;",
  "thi-ap17:95.3_0": "Anuppattā sakaṁ atthaṁ,",
  "thi-ap17:95.4_0": "pāde vandāmi cakkhuma”.",
  "thi-ap17:96.1_0": "Dassetvā vividhā iddhī,",
  "thi-ap17:96.2_0": "orohitvā nabhattalā;",
  "thi-ap17:96.3_0": "Vanditvā lokapajjotaṁ,",
  "thi-ap17:96.4_0": "ekamantaṁ nisīdi sā.",
  "thi-ap17:97.1_0": "“Sā vīsavassasatikā,",
  "thi-ap17:97.2_0": "jātiyāhaṁ mahāmune;",
  "thi-ap17:97.3_0": "Alamettāvatā vīra,",
  "thi-ap17:97.4_0": "nibbāyissāmi nāyaka”.",
  "thi-ap17:98.1_0": "Tadātivimhitā sabbā,",
  "thi-ap17:98.2_0": "parisā sā katañjalī;",
  "thi-ap17:98.3_0": "“Avocayye kathaṁ āsi,",
  "thi-ap17:98.4_0": "atuliddhiparakkamā”.",
  "thi-ap17:99.1_0": "“Padumuttaro nāma jino,",
  "thi-ap17:99.2_0": "sabbadhammesu cakkhumā;",
  "thi-ap17:99.3_0": "Ito satasahassamhi,",
  "thi-ap17:99.4_0": "kappe uppajji nāyako.",
  "thi-ap17:100.1_0": "Tadāhaṁ haṁsavatiyaṁ,",
  "thi-ap17:100.2_0": "jātāmaccakule ahuṁ;",
  "thi-ap17:100.3_0": "Sabbopakārasampanne,",
  "thi-ap17:100.4_0": "iddhe phīte mahaddhane.",
  "thi-ap17:101.1_0": "Kadāci pitunā saddhiṁ,",
  "thi-ap17:101.2_0": "dāsīgaṇapurakkhatā;",
  "thi-ap17:101.3_0": "Mahatā parivārena,",
  "thi-ap17:101.4_0": "taṁ upecca narāsabhaṁ.",
  "thi-ap17:102.1_0": "Vāsavaṁ viya vassantaṁ,",
  "thi-ap17:102.2_0": "dhammameghaṁ anāsavaṁ;",
  "thi-ap17:102.3_0": "Saradādiccasadisaṁ,",
  "thi-ap17:102.4_0": "raṁsijālasamujjalaṁ.",
  "thi-ap17:103.1_0": "Disvā cittaṁ pasādetvā,",
  "thi-ap17:103.2_0": "sutvā cassa subhāsitaṁ;",
  "thi-ap17:103.3_0": "Mātucchaṁ bhikkhuniṁ agge,",
  "thi-ap17:103.4_0": "ṭhapentaṁ naranāyakaṁ.",
  "thi-ap17:104.1_0": "Sutvā datvā mahādānaṁ,",
  "thi-ap17:104.2_0": "sattāhaṁ tassa tādino;",
  "thi-ap17:104.3_0": "Sasaṅghassa naraggassa,",
  "thi-ap17:104.4_0": "paccayāni bahūni ca.",
  "thi-ap17:105.1_0": "Nipacca pādamūlamhi,",
  "thi-ap17:105.2_0": "taṁ ṭhānamabhipatthayiṁ;",
  "thi-ap17:105.3_0": "Tato mahāparisatiṁ,",
  "thi-ap17:105.4_0": "avoca isisattamo.",
  "thi-ap17:106.1_0": "‘Yā sasaṅghaṁ abhojesi,",
  "thi-ap17:106.2_0": "sattāhaṁ lokanāyakaṁ;",
  "thi-ap17:106.3_0": "Tamahaṁ kittayissāmi,",
  "thi-ap17:106.4_0": "suṇātha mama bhāsato.",
  "thi-ap17:107.1_0": "Satasahassito kappe,",
  "thi-ap17:107.2_0": "okkākakulasambhavo;",
  "thi-ap17:107.3_0": "Gotamo nāma gottena,",
  "thi-ap17:107.4_0": "satthā loke bhavissati.",
  "thi-ap17:108.1_0": "Tassa dhammesu dāyādā,",
  "thi-ap17:108.2_0": "orasā dhammanimmitā;",
  "thi-ap17:108.3_0": "Gotamī nāma nāmena,",
  "thi-ap17:108.4_0": "hessati satthu sāvikā.",
  "thi-ap17:109.1_0": "Tassa buddhassa mātucchā,",
  "thi-ap17:109.2_0": "jīvitāpādikā ayaṁ;",
  "thi-ap17:109.3_0": "Rattaññūnañca aggattaṁ,",
  "thi-ap17:109.4_0": "bhikkhunīnaṁ labhissati’.",
  "thi-ap17:110.1_0": "Taṁ sutvāna pamoditvā,",
  "thi-ap17:110.2_0": "yāvajīvaṁ tadā jinaṁ;",
  "thi-ap17:110.3_0": "Paccayehi upaṭṭhitvā,",
  "thi-ap17:110.4_0": "tato kālaṅkatā ahaṁ.",
  "thi-ap17:111.1_0": "Tāvatiṁsesu devesu,",
  "thi-ap17:111.2_0": "sabbakāmasamiddhisu;",
  "thi-ap17:111.3_0": "Nibbattā dasahaṅgehi,",
  "thi-ap17:111.4_0": "aññe abhibhaviṁ ahaṁ.",
  "thi-ap17:112.1_0": "Rūpasaddehi gandhehi,",
  "thi-ap17:112.2_0": "rasehi phusanehi ca;",
  "thi-ap17:112.3_0": "Āyunāpi ca vaṇṇena,",
  "thi-ap17:112.4_0": "sukhena yasasāpi ca.",
  "thi-ap17:113.1_0": "Tathevādhipateyyena,",
  "thi-ap17:113.2_0": "adhigayha virocahaṁ;",
  "thi-ap17:113.3_0": "Ahosiṁ amarindassa,",
  "thi-ap17:113.4_0": "mahesī dayitā tahiṁ.",
  "thi-ap17:114.1_0": "Saṁsāre saṁsarantīhaṁ,",
  "thi-ap17:114.2_0": "kammavāyusameritā;",
  "thi-ap17:114.3_0": "Kāsissa rañño visaye,",
  "thi-ap17:114.4_0": "ajāyiṁ dāsagāmake.",
  "thi-ap17:115.1_0": "Pañcadāsasatānūnā,",
  "thi-ap17:115.2_0": "nivasanti tahiṁ tadā;",
  "thi-ap17:115.3_0": "Sabbesaṁ tattha yo jeṭṭho,",
  "thi-ap17:115.4_0": "tassa jāyā ahosahaṁ.",
  "thi-ap17:116.1_0": "Sayambhuno pañcasatā,",
  "thi-ap17:116.2_0": "gāmaṁ piṇḍāya pāvisuṁ;",
  "thi-ap17:116.3_0": "Te disvāna ahaṁ tuṭṭhā,",
  "thi-ap17:116.4_0": "saha sabbāhi itthibhi.",
  "thi-ap17:117.1_0": "Pūgā hutvāva sabbāyo,",
  "thi-ap17:117.2_0": "catumāse upaṭṭhahuṁ;",
  "thi-ap17:117.3_0": "Ticīvarāni datvāna,",
  "thi-ap17:117.4_0": "saṁsarimha sasāmikā.",
  "thi-ap17:118.1_0": "Tato cutā sabbāpi tā,",
  "thi-ap17:118.2_0": "tāvatiṁsagatā mayaṁ;",
  "thi-ap17:118.3_0": "Pacchime ca bhave dāni,",
  "thi-ap17:118.4_0": "jātā devadahe pure.",
  "thi-ap17:119.1_0": "Pitā añjanasakko me,",
  "thi-ap17:119.2_0": "mātā mama sulakkhaṇā;",
  "thi-ap17:119.3_0": "Tato kapilavatthusmiṁ,",
  "thi-ap17:119.4_0": "suddhodanagharaṁ gatā.",
  "thi-ap17:120.1_0": "Sesā sakyakule jātā,",
  "thi-ap17:120.2_0": "sakyānaṁ gharamāgamuṁ;",
  "thi-ap17:120.3_0": "Ahaṁ visiṭṭhā sabbāsaṁ,",
  "thi-ap17:120.4_0": "jinassāpādikā ahuṁ.",
  "thi-ap17:121.1_0": "Mama puttobhinikkhamma,",
  "thi-ap17:121.2_0": "buddho āsi vināyako;",
  "thi-ap17:121.3_0": "Pacchāhaṁ pabbajitvāna,",
  "thi-ap17:121.4_0": "satehi saha pañcahi.",
  "thi-ap17:122.1_0": "Sākiyānīhi dhīrāhi,",
  "thi-ap17:122.2_0": "saha santisukhaṁ phusiṁ;",
  "thi-ap17:122.3_0": "Ye tadā pubbajātiyaṁ,",
  "thi-ap17:122.4_0": "amhākaṁ āsu sāmino.",
  "thi-ap17:123.1_0": "Sahapuññassa kattāro,",
  "thi-ap17:123.2_0": "mahāsamayakārakā;",
  "thi-ap17:123.3_0": "Phusiṁsu arahattaṁ te,",
  "thi-ap17:123.4_0": "sugatenānukampitā”.",
  "thi-ap17:124.1_0": "Tadetarā bhikkhuniyo,",
  "thi-ap17:124.2_0": "āruhiṁsu nabhattalaṁ;",
  "thi-ap17:124.3_0": "Saṅgatā viya tārāyo,",
  "thi-ap17:124.4_0": "virociṁsu mahiddhikā.",
  "thi-ap17:125.1_0": "Iddhī anekā dassesuṁ,",
  "thi-ap17:125.2_0": "piḷandhavikatiṁ yathā;",
  "thi-ap17:125.3_0": "Kammāro kanakasseva,",
  "thi-ap17:125.4_0": "kammaññassa susikkhito.",
  "thi-ap17:126.1_0": "Dassetvā pāṭihīrāni,",
  "thi-ap17:126.2_0": "vicittāni bahūni ca;",
  "thi-ap17:126.3_0": "Tosetvā vādipavaraṁ,",
  "thi-ap17:126.4_0": "muniṁ saparisaṁ tadā.",
  "thi-ap17:127.1_0": "Orohitvāna gaganā,",
  "thi-ap17:127.2_0": "vanditvā isisattamaṁ;",
  "thi-ap17:127.3_0": "Anuññātā naraggena,",
  "thi-ap17:127.4_0": "yathāṭhāne nisīdisuṁ.",
  "thi-ap17:128.1_0": "“Ahonukampikā amhaṁ,",
  "thi-ap17:128.2_0": "sabbāsaṁ cira gotamī;",
  "thi-ap17:128.3_0": "Vāsitā tava puññehi,",
  "thi-ap17:128.4_0": "pattā no āsavakkhayaṁ.",
  "thi-ap17:129.1_0": "Kilesā jhāpitā amhaṁ,",
  "thi-ap17:129.2_0": "bhavā sabbe samūhatā;",
  "thi-ap17:129.3_0": "Nāgīva bandhanaṁ chetvā,",
  "thi-ap17:129.4_0": "viharāma anāsavā.",
  "thi-ap17:130.1_0": "Svāgataṁ vata no āsi,",
  "thi-ap17:130.2_0": "buddhaseṭṭhassa santike;",
  "thi-ap17:130.3_0": "Tisso vijjā anuppattā,",
  "thi-ap17:130.4_0": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap17:131.1_0": "Paṭisambhidā catasso,",
  "thi-ap17:131.2_0": "vimokkhāpi ca aṭṭhime;",
  "thi-ap17:131.3_0": "Chaḷabhiññā sacchikatā,",
  "thi-ap17:131.4_0": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap17:132.1_0": "Iddhīsu ca vasī homa,",
  "thi-ap17:132.2_0": "dibbāya sotadhātuyā;",
  "thi-ap17:132.3_0": "Cetopariyañāṇassa,",
  "thi-ap17:132.4_0": "vasī homa mahāmune.",
  "thi-ap17:133.1_0": "Pubbenivāsaṁ jānāma,",
  "thi-ap17:133.2_0": "dibbacakkhu visodhitaṁ;",
  "thi-ap17:133.3_0": "Sabbāsavaparikkhīṇā,",
  "thi-ap17:133.4_0": "natthi dāni punabbhavā.",
  "thi-ap17:134.1_0": "Atthe dhamme ca nerutte,",
  "thi-ap17:134.2_0": "paṭibhāne ca vijjati;",
  "thi-ap17:134.3_0": "Ñāṇaṁ amhaṁ mahāvīra,",
  "thi-ap17:134.4_0": "uppannaṁ tava santike.",
  "thi-ap17:135.1_0": "Asmābhi pariciṇṇosi,",
  "thi-ap17:135.2_0": "mettacittā hi nāyaka;",
  "thi-ap17:135.3_0": "Anujānāhi sabbāsaṁ,",
  "thi-ap17:135.4_0": "nibbānāya mahāmune”.",
  "thi-ap17:136.1_0": "“Nibbāyissāma iccevaṁ,",
  "thi-ap17:136.2_0": "kiṁ vakkhāmi vadantiyo;",
  "thi-ap17:136.3_0": "Yassadāni ca vo kālaṁ,",
  "thi-ap17:136.4_0": "maññathā”ti jinobravi.",
  "thi-ap17:137.1_0": "Gotamīādikā tāyo,",
  "thi-ap17:137.2_0": "tadā bhikkhuniyo jinaṁ;",
  "thi-ap17:137.3_0": "Vanditvā āsanā tamhā,",
  "thi-ap17:137.4_0": "vuṭṭhāya agamiṁsu tā.",
  "thi-ap17:138.1_0": "Mahatā janakāyena,",
  "thi-ap17:138.2_0": "saha lokagganāyako;",
  "thi-ap17:138.3_0": "Anusaṁyāyī so vīro,",
  "thi-ap17:138.4_0": "mātucchaṁ yāvakoṭṭhakaṁ.",
  "thi-ap17:139.1_0": "Tadā nipati pādesu,",
  "thi-ap17:139.2_0": "gotamī lokabandhuno;",
  "thi-ap17:139.3_0": "Saheva tāhi sabbāhi,",
  "thi-ap17:139.4_0": "“pacchimaṁ pādavandanaṁ.",
  "thi-ap17:140.1_0": "Idaṁ pacchimakaṁ mayhaṁ,",
  "thi-ap17:140.2_0": "lokanāthassa dassanaṁ;",
  "thi-ap17:140.3_0": "Na puno amatākāraṁ,",
  "thi-ap17:140.4_0": "passissāmi mukhaṁ tava.",
  "thi-ap17:141.1_0": "Na ca me vandanaṁ vīra,",
  "thi-ap17:141.2_0": "tava pāde sukomale;",
  "thi-ap17:141.3_0": "Samphusissati lokagga,",
  "thi-ap17:141.4_0": "ajja gacchāmi nibbutiṁ”.",
  "thi-ap17:142.1_0": "“Rūpena kiṁ tavānena,",
  "thi-ap17:142.2_0": "diṭṭhe dhamme yathātathe;",
  "thi-ap17:142.3_0": "Sabbaṁ saṅkhatamevetaṁ,",
  "thi-ap17:142.4_0": "anassāsikamittaraṁ”.",
  "thi-ap17:143.1_0": "Sā saha tāhi gantvāna,",
  "thi-ap17:143.2_0": "bhikkhunupassayaṁ sakaṁ;",
  "thi-ap17:143.3_0": "Aḍḍhapallaṅkamābhujja,",
  "thi-ap17:143.4_0": "nisīdi paramāsane.",
  "thi-ap17:144.1_0": "Tadā upāsikā tattha,",
  "thi-ap17:144.2_0": "buddhasāsanavacchalā;",
  "thi-ap17:144.3_0": "Tassā pavattiṁ sutvāna,",
  "thi-ap17:144.4_0": "upesuṁ pādavandikā.",
  "thi-ap17:145.1_0": "Karehi uraṁ pahantā,",
  "thi-ap17:145.2_0": "chinnamūlā yathā latā;",
  "thi-ap17:145.3_0": "Rodantā karuṇaṁ ravaṁ,",
  "thi-ap17:145.4_0": "sokaṭṭā bhūmipātitā.",
  "thi-ap17:146.1_0": "“Mā no saraṇade nāthe,",
  "thi-ap17:146.2_0": "vihāya gami nibbutiṁ;",
  "thi-ap17:146.3_0": "Nipatitvāna yācāma,",
  "thi-ap17:146.4_0": "sabbāyo sirasā mayaṁ”.",
  "thi-ap17:147.1_0": "Yā padhānatamā tāsaṁ,",
  "thi-ap17:147.2_0": "saddhā paññā upāsikā;",
  "thi-ap17:147.3_0": "Tassā sīsaṁ pamajjantī,",
  "thi-ap17:147.4_0": "idaṁ vacanamabravi.",
  "thi-ap17:148.1_0": "“Alaṁ puttā visādena,",
  "thi-ap17:148.2_0": "mārapāsānuvattinā;",
  "thi-ap17:148.3_0": "Aniccaṁ saṅkhataṁ sabbaṁ,",
  "thi-ap17:148.4_0": "viyogantaṁ calācalaṁ”.",
  "thi-ap17:149.1_0": "Tato sā tā visajjitvā,",
  "thi-ap17:149.2_0": "paṭhamaṁ jhānamuttamaṁ;",
  "thi-ap17:149.3_0": "Dutiyañca tatiyañca,",
  "thi-ap17:149.4_0": "samāpajji catutthakaṁ.",
  "thi-ap17:150.1_0": "Ākāsāyatanañceva,",
  "thi-ap17:150.2_0": "viññāṇāyatanaṁ tathā;",
  "thi-ap17:150.3_0": "Ākiñcaṁ nevasaññañca,",
  "thi-ap17:150.4_0": "samāpajji yathākkamaṁ.",
  "thi-ap17:151.1_0": "Paṭilomena jhānāni,",
  "thi-ap17:151.2_0": "samāpajjittha gotamī;",
  "thi-ap17:151.3_0": "Yāvatā paṭhamaṁ jhānaṁ,",
  "thi-ap17:151.4_0": "tato yāvacatutthakaṁ.",
  "thi-ap17:152.1_0": "Tato vuṭṭhāya nibbāyi,",
  "thi-ap17:152.2_0": "dīpaccīva nirāsavā;",
  "thi-ap17:152.3_0": "Bhūmicālo mahā āsi,",
  "thi-ap17:152.4_0": "nabhasā vijjutā pati.",
  "thi-ap17:153.1_0": "Panāditā dundubhiyo,",
  "thi-ap17:153.2_0": "parideviṁsu devatā;",
  "thi-ap17:153.3_0": "Pupphavuṭṭhī ca gaganā,",
  "thi-ap17:153.4_0": "abhivassatha medaniṁ.",
  "thi-ap17:154.1_0": "Kampito merurājāpi,",
  "thi-ap17:154.2_0": "raṅgamajjhe yathā naṭo;",
  "thi-ap17:154.3_0": "Sokena cātidīnova,",
  "thi-ap17:154.4_0": "viravo āsi sāgaro.",
  "thi-ap17:155.1_0": "Devā nāgāsurā brahmā,",
  "thi-ap17:155.2_0": "saṁviggāhiṁsu taṅkhaṇe;",
  "thi-ap17:155.3_0": "“Aniccā vata saṅkhārā,",
  "thi-ap17:155.4_0": "yathāyaṁ vilayaṁ gatā”.",
  "thi-ap17:156.1_0": "Yā ce maṁ parivāriṁsu,",
  "thi-ap17:156.2_0": "satthu sāsanakārikā;",
  "thi-ap17:156.3_0": "Tāyopi anupādānā,",
  "thi-ap17:156.4_0": "dīpacci viya nibbutā.",
  "thi-ap17:157.1_0": "Hā yogā vippayogantā,",
  "thi-ap17:157.2_0": "hāniccaṁ sabbasaṅkhataṁ;",
  "thi-ap17:157.3_0": "Hā jīvitaṁ vināsantaṁ,",
  "thi-ap17:157.4_0": "iccāsi paridevanā.",
  "thi-ap17:158.1_0": "Tato devā ca brahmā ca,",
  "thi-ap17:158.2_0": "lokadhammānuvattanaṁ;",
  "thi-ap17:158.3_0": "Kālānurūpaṁ kubbanti,",
  "thi-ap17:158.4_0": "upetvā isisattamaṁ.",
  "thi-ap17:159.1_0": "Tadā āmantayī satthā,",
  "thi-ap17:159.2_0": "ānandaṁ sutasāgaraṁ;",
  "thi-ap17:159.3_0": "“Gacchānanda nivedehi,",
  "thi-ap17:159.4_0": "bhikkhūnaṁ mātu nibbutiṁ”.",
  "thi-ap17:160.1_0": "Tadānando nirānando,",
  "thi-ap17:160.2_0": "assunā puṇṇalocano;",
  "thi-ap17:160.3_0": "Gaggarena sarenāha,",
  "thi-ap17:160.4_0": "“samāgacchantu bhikkhavo.",
  "thi-ap17:161.1_0": "Pubbadakkhiṇapacchāsu,",
  "thi-ap17:161.2_0": "uttarāya ca santike;",
  "thi-ap17:161.3_0": "Suṇantu bhāsitaṁ mayhaṁ,",
  "thi-ap17:161.4_0": "bhikkhavo sugatorasā.",
  "thi-ap17:162.1_0": "Yā vaḍḍhayi payattena,",
  "thi-ap17:162.2_0": "sarīraṁ pacchimaṁ mune;",
  "thi-ap17:162.3_0": "Sā gotamī gatā santiṁ,",
  "thi-ap17:162.4_0": "tārāva sūriyodaye.",
  "thi-ap17:163.1_0": "Buddhamātāti paññattiṁ,",
  "thi-ap17:163.2_0": "ṭhapayitvā gatāsamaṁ;",
  "thi-ap17:163.3_0": "Na yattha pañcanettopi,",
  "thi-ap17:163.4_0": "gatiṁ dakkhati nāyako.",
  "thi-ap17:164.1_0": "Yassatthi sugate saddhā,",
  "thi-ap17:164.2_0": "yo ca piyo mahāmune;",
  "thi-ap17:164.3_0": "Buddhamātussa sakkāraṁ,",
  "thi-ap17:164.4_0": "karotu sugatoraso”.",
  "thi-ap17:165.1_0": "Sudūraṭṭhāpi taṁ sutvā,",
  "thi-ap17:165.2_0": "sīghamāgacchu bhikkhavo;",
  "thi-ap17:165.3_0": "Keci buddhānubhāvena,",
  "thi-ap17:165.4_0": "keci iddhīsu kovidā.",
  "thi-ap17:166.1_0": "Kūṭāgāravare ramme,",
  "thi-ap17:166.2_0": "sabbasoṇṇamaye subhe;",
  "thi-ap17:166.3_0": "Mañcakaṁ samāropesuṁ,",
  "thi-ap17:166.4_0": "yattha suttāsi gotamī.",
  "thi-ap17:167.1_0": "Cattāro lokapālā te,",
  "thi-ap17:167.2_0": "aṁsehi samadhārayuṁ;",
  "thi-ap17:167.3_0": "Sesā sakkādikā devā,",
  "thi-ap17:167.4_0": "kūṭāgāre samaggahuṁ.",
  "thi-ap17:168.1_0": "Kūṭāgārāni sabbāni,",
  "thi-ap17:168.2_0": "āsuṁ pañcasatānipi;",
  "thi-ap17:168.3_0": "Saradādiccavaṇṇāni,",
  "thi-ap17:168.4_0": "vissakammakatāni hi.",
  "thi-ap17:169.1_0": "Sabbā tāpi bhikkhuniyo,",
  "thi-ap17:169.2_0": "āsuṁ mañcesu sāyitā;",
  "thi-ap17:169.3_0": "Devānaṁ khandhamāruḷhā,",
  "thi-ap17:169.4_0": "niyyanti anupubbaso.",
  "thi-ap17:170.1_0": "Sabbaso chāditaṁ āsi,",
  "thi-ap17:170.2_0": "vitānena nabhattalaṁ;",
  "thi-ap17:170.3_0": "Satārā candasūrā ca,",
  "thi-ap17:170.4_0": "lañchitā kanakāmayā.",
  "thi-ap17:171.1_0": "Paṭākā ussitānekā,",
  "thi-ap17:171.2_0": "vitatā pupphakañcukā;",
  "thi-ap17:171.3_0": "Ogatākāsapadumā,",
  "thi-ap17:171.4_0": "mahiyā pupphamuggataṁ.",
  "thi-ap17:172.1_0": "Dissanti candasūriyā,",
  "thi-ap17:172.2_0": "pajjalanti ca tārakā;",
  "thi-ap17:172.3_0": "Majjhaṁ gatopi cādicco,",
  "thi-ap17:172.4_0": "na tāpesi sasī yathā.",
  "thi-ap17:173.1_0": "Devā dibbehi gandhehi,",
  "thi-ap17:173.2_0": "mālehi surabhīhi ca;",
  "thi-ap17:173.3_0": "Vāditehi ca naccehi,",
  "thi-ap17:173.4_0": "saṅgītīhi ca pūjayuṁ.",
  "thi-ap17:174.1_0": "Nāgāsurā ca brahmāno,",
  "thi-ap17:174.2_0": "yathāsatti yathābalaṁ;",
  "thi-ap17:174.3_0": "Pūjayiṁsu ca niyyantiṁ,",
  "thi-ap17:174.4_0": "nibbutaṁ buddhamātaraṁ.",
  "thi-ap17:175.1_0": "Sabbāyo purato nītā,",
  "thi-ap17:175.2_0": "nibbutā sugatorasā;",
  "thi-ap17:175.3_0": "Gotamī niyyate pacchā,",
  "thi-ap17:175.4_0": "sakkatā buddhaposikā.",
  "thi-ap17:176.1_0": "Purato devamanujā,",
  "thi-ap17:176.2_0": "sanāgāsurabrahmakā;",
  "thi-ap17:176.3_0": "Pacchā sasāvako buddho,",
  "thi-ap17:176.4_0": "pūjatthaṁ yāti mātuyā.",
  "thi-ap17:177.1_0": "Buddhassa parinibbānaṁ,",
  "thi-ap17:177.2_0": "nedisaṁ āsi yādisaṁ;",
  "thi-ap17:177.3_0": "Gotamīparinibbānaṁ,",
  "thi-ap17:177.4_0": "atevacchariyaṁ ahu.",
  "thi-ap17:178.1_0": "Buddho buddhassa nibbāne,",
  "thi-ap17:178.2_0": "nopaṭiyādi bhikkhavo;",
  "thi-ap17:178.3_0": "Buddho gotaminibbāne,",
  "thi-ap17:178.4_0": "sāriputtādikā tathā.",
  "thi-ap17:179.1_0": "Citakāni karitvāna,",
  "thi-ap17:179.2_0": "sabbagandhamayāni te;",
  "thi-ap17:179.3_0": "Gandhacuṇṇapakiṇṇāni,",
  "thi-ap17:179.4_0": "jhāpayiṁsu ca tā tahiṁ.",
  "thi-ap17:180.1_0": "Sesabhāgāni ḍayhiṁsu,",
  "thi-ap17:180.2_0": "aṭṭhī sesāni sabbaso;",
  "thi-ap17:180.3_0": "Ānando ca tadāvoca,",
  "thi-ap17:180.4_0": "saṁvegajanakaṁ vaco.",
  "thi-ap17:181.1_0": "“Gotamī nidhanaṁ yātā,",
  "thi-ap17:181.2_0": "ḍayhañcassā sarīrakaṁ;",
  "thi-ap17:181.3_0": "Saṅketaṁ buddhanibbānaṁ,",
  "thi-ap17:181.4_0": "na cirena bhavissati”.",
  "thi-ap17:182.1_0": "Tato gotamidhātūni,",
  "thi-ap17:182.2_0": "tassā pattagatāni so;",
  "thi-ap17:182.3_0": "Upanāmesi nāthassa,",
  "thi-ap17:182.4_0": "ānando buddhacodito.",
  "thi-ap17:183.1_0": "Pāṇinā tāni paggayha,",
  "thi-ap17:183.2_0": "avoca isisattamo;",
  "thi-ap17:183.3_0": "“Mahato sāravantassa,",
  "thi-ap17:183.4_0": "yathā rukkhassa tiṭṭhato.",
  "thi-ap17:184.1_0": "Yo so mahattaro khandho,",
  "thi-ap17:184.2_0": "palujjeyya aniccatā;",
  "thi-ap17:184.3_0": "Tathā bhikkhunisaṅghassa,",
  "thi-ap17:184.4_0": "gotamī parinibbutā.",
  "thi-ap17:185.1_0": "Aho acchariyaṁ mayhaṁ,",
  "thi-ap17:185.2_0": "nibbutāyapi mātuyā;",
  "thi-ap17:185.3_0": "Sarīramattasesāya,",
  "thi-ap17:185.4_0": "natthi sokapariddavo.",
  "thi-ap17:186.1_0": "Na sociyā paresaṁ sā,",
  "thi-ap17:186.2_0": "tiṇṇasaṁsārasāgarā;",
  "thi-ap17:186.3_0": "Parivajjitasantāpā,",
  "thi-ap17:186.4_0": "sītibhūtā sunibbutā.",
  "thi-ap17:187.1_0": "Paṇḍitāsi mahāpaññā,",
  "thi-ap17:187.2_0": "puthupaññā tatheva ca;",
  "thi-ap17:187.3_0": "Rattaññū bhikkhunīnaṁ sā,",
  "thi-ap17:187.4_0": "evaṁ dhāretha bhikkhavo.",
  "thi-ap17:188.1_0": "Iddhīsu ca vasī āsi,",
  "thi-ap17:188.2_0": "dibbāya sotadhātuyā;",
  "thi-ap17:188.3_0": "Cetopariyañāṇassa,",
  "thi-ap17:188.4_0": "vasī āsi ca gotamī.",
  "thi-ap17:189.1_0": "Pubbenivāsamaññāsi,",
  "thi-ap17:189.2_0": "dibbacakkhu visodhitaṁ;",
  "thi-ap17:189.3_0": "Sabbāsavā parikkhīṇā,",
  "thi-ap17:189.4_0": "natthi tassā punabbhavo.",
  "thi-ap17:190.1_0": "Atthadhammaniruttīsu,",
  "thi-ap17:190.2_0": "paṭibhāne tatheva ca;",
  "thi-ap17:190.3_0": "Parisuddhaṁ ahu ñāṇaṁ,",
  "thi-ap17:190.4_0": "tasmā socaniyā na sā.",
  "thi-ap17:191.1_0": "Ayoghanahatasseva,",
  "thi-ap17:191.2_0": "jalato jātavedassa;",
  "thi-ap17:191.3_0": "Anupubbūpasantassa,",
  "thi-ap17:191.4_0": "yathā na ñāyate gati.",
  "thi-ap17:192.1_0": "Evaṁ sammā vimuttānaṁ,",
  "thi-ap17:192.2_0": "kāmabandhoghatārinaṁ;",
  "thi-ap17:192.3_0": "Paññāpetuṁ gati natthi,",
  "thi-ap17:192.4_0": "pattānaṁ acalaṁ sukhaṁ.",
  "thi-ap17:193.1_0": "Attadīpā tato hotha,",
  "thi-ap17:193.2_0": "satipaṭṭhānagocarā;",
  "thi-ap17:193.3_0": "Bhāvetvā sattabojjhaṅge,",
  "thi-ap17:193.4_0": "dukkhassantaṁ karissatha”.",
  "thi-ap17:194.1_0": "Itthaṁ sudaṁ mahāpajāpatigotamī imā gāthāyo abhāsitthāti.",
  "thi-ap17:195.1_0": "Mahāpajāpatigotamītheriyāpadānaṁ sattamaṁ."
}