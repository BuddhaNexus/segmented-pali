{
  "anya-e1004n3:0_0": "Nāyakakathā",
  "anya-e1004n3:1_0": "164.",
  "anya-e1004n3:2_0": "Kassako vāṇijomacco,",
  "anya-e1004n3:3_0": "Samaṇo sutasīlavā;",
  "anya-e1004n3:4_0": "Tesu vipulajātesu,",
  "anya-e1004n3:5_0": "Raṭṭhaṁpi vipulaṁ siyā.",
  "anya-e1004n3:6_0": "165.",
  "anya-e1004n3:7_0": "Tesu dubbalajātesu,",
  "anya-e1004n3:8_0": "Raṭṭhaṁpi dubbalaṁ siyā;",
  "anya-e1004n3:9_0": "Tasmā raṭṭhaṁpi vipulaṁ,",
  "anya-e1004n3:10_0": "Dhāraye raṭṭhasāravā.",
  "anya-e1004n3:11_0": "166.",
  "anya-e1004n3:12_0": "Mahārukkhassa phalino,",
  "anya-e1004n3:13_0": "Āmaṁ chindati yo phalaṁ;",
  "anya-e1004n3:14_0": "Rasañcassa najānāti,",
  "anya-e1004n3:15_0": "Bījañcassa vinassati.",
  "anya-e1004n3:16_0": "167.",
  "anya-e1004n3:17_0": "Mahārukkhupamaṁ raṭṭhaṁ,",
  "anya-e1004n3:18_0": "Yo adhammena sāsati;",
  "anya-e1004n3:19_0": "Rasañcassa najānāti,",
  "anya-e1004n3:20_0": "Raṭṭhañcāpi vinassati.",
  "anya-e1004n3:21_0": "168.",
  "anya-e1004n3:22_0": "Mahārukkhassa phalino,",
  "anya-e1004n3:23_0": "Pakkaṁ chindati yo phalaṁ;",
  "anya-e1004n3:24_0": "Rasañcassa vijānāti,",
  "anya-e1004n3:25_0": "Bījañcassa nanassati.",
  "anya-e1004n3:26_0": "169.",
  "anya-e1004n3:27_0": "Mahārukkhupamaṁ raṭṭhaṁ,",
  "anya-e1004n3:28_0": "Dhammena yo pasāsati;",
  "anya-e1004n3:29_0": "Rasañcassa vijānāti,",
  "anya-e1004n3:30_0": "Raṭṭhañcāpi nanassati.",
  "anya-e1004n3:31_0": "170.",
  "anya-e1004n3:32_0": "Janappadañca yorājā,",
  "anya-e1004n3:33_0": "Adhammena pasāsati;",
  "anya-e1004n3:34_0": "Sabbo sadhihi sorājā,",
  "anya-e1004n3:35_0": "Viruddho hoti khattiyo.",
  "anya-e1004n3:36_0": "171.",
  "anya-e1004n3:37_0": "Tatheva nigame hiṁsaṁ,",
  "anya-e1004n3:38_0": "Ye yuttā kayavikkaye;",
  "anya-e1004n3:39_0": "Suṅkadānabalīkāre,",
  "anya-e1004n3:40_0": "Sa kosena virujjhati.",
  "anya-e1004n3:41_0": "172.",
  "anya-e1004n3:42_0": "Mahāravarakhettesu,",
  "anya-e1004n3:43_0": "Saṅga, me katanissame;",
  "anya-e1004n3:44_0": "Ussite hiṁsayaṁ rājā,",
  "anya-e1004n3:45_0": "Sabalena virujjhati.",
  "anya-e1004n3:46_0": "173.",
  "anya-e1004n3:47_0": "Tatheva isayo hiṁso,",
  "anya-e1004n3:48_0": "Saṁyame brahmacāriyo;",
  "anya-e1004n3:49_0": "Adhammacāri khattīyo,",
  "anya-e1004n3:50_0": "So saggena virujjhati.",
  "anya-e1004n3:51_0": "174.",
  "anya-e1004n3:52_0": "Sayaṁkatā naparena,",
  "anya-e1004n3:53_0": "Mahānajjo juvakatā;",
  "anya-e1004n3:54_0": "Issarena tathā raññā,",
  "anya-e1004n3:55_0": "Saraṭṭhe adhipaccatā.",
  "anya-e1004n3:56_0": "175.",
  "anya-e1004n3:57_0": "Pāpaṁvāpi hi puññaṁvā,",
  "anya-e1004n3:58_0": "Padhāno yaṁ karotice;",
  "anya-e1004n3:59_0": "Lokopevaṁ karotyeva,",
  "anya-e1004n3:60_0": "Taṁ vijāneyya paṇḍito.",
  "anya-e1004n3:61_0": "176.",
  "anya-e1004n3:62_0": "Kacchapīnañca macchīnaṁ,",
  "anya-e1004n3:63_0": "Kukkuṭīnañca dhenūnaṁ;",
  "anya-e1004n3:64_0": "Puttaposā yathāhoti,",
  "anya-e1004n3:65_0": "Tathā maccesu rājūnaṁ.",
  "anya-e1004n3:66_0": "177.",
  "anya-e1004n3:67_0": "Anāyakā vinassanti,",
  "anya-e1004n3:68_0": "Nassanti bahunāyakā;",
  "anya-e1004n3:69_0": "Thīnāyakā vinassanti,",
  "anya-e1004n3:70_0": "Nassanti susunāyakā.",
  "anya-e1004n3:71_0": "178.",
  "anya-e1004n3:72_0": "Bahuvo yattha netāro,",
  "anya-e1004n3:73_0": "Sabbe paṇḍitamānino;",
  "anya-e1004n3:74_0": "Sabbe mahattamicchanti,",
  "anya-e1004n3:75_0": "Kataṁ nesaṁ vinassati.",
  "anya-e1004n3:76_0": "179.",
  "anya-e1004n3:77_0": "Nodayāya vināsāya,",
  "anya-e1004n3:78_0": "Bahunāyakatā bhūsaṁ;",
  "anya-e1004n3:79_0": "Nomilanti vinassanti,",
  "anya-e1004n3:80_0": "Padmā nyakkehisattahi.",
  "anya-e1004n3:81_0": "180.",
  "anya-e1004n3:82_0": "Asantuṭṭho yatī naṭṭho,",
  "anya-e1004n3:83_0": "Santuṭṭhoca mahīpati;",
  "anya-e1004n3:84_0": "Salajjā gaṇikā naṭṭhā,",
  "anya-e1004n3:85_0": "Nilajjātu kulaṅganā.",
  "anya-e1004n3:86_0": "181.",
  "anya-e1004n3:87_0": "Na gaṇassa ggato gacche,",
  "anya-e1004n3:88_0": "Siddhe kamme samaṁphalaṁ;",
  "anya-e1004n3:89_0": "Kammavippatti cehoti,",
  "anya-e1004n3:90_0": "Mukharo tatra haññate.",
  "anya-e1004n3:91_0": "182.",
  "anya-e1004n3:92_0": "Padhiro ca tapassīni,",
  "anya-e1004n3:93_0": "Sūro raṇavaṇo tathā;",
  "anya-e1004n3:94_0": "Majjapo patithī rājā,",
  "anya-e1004n3:95_0": "Ete nasaddahāmahaṁ.",
  "anya-e1004n3:96_0": "183.",
  "anya-e1004n3:97_0": "Jāneyya pesane bhaccaṁ,",
  "anya-e1004n3:98_0": "Bandhavāpi bhayāgate;",
  "anya-e1004n3:99_0": "Āpadāsu tathā mittaṁ,",
  "anya-e1004n3:100_0": "Dārañca vibhavakkhaye.",
  "anya-e1004n3:101_0": "184.",
  "anya-e1004n3:102_0": "Raṇā paccāgataṁ sūraṁ,",
  "anya-e1004n3:103_0": "Dhanañca gharamāgataṁ;",
  "anya-e1004n3:104_0": "Jiṇṇa mannaṁ pasaṁseyya,",
  "anya-e1004n3:105_0": "Dārañca gatayobbanaṁ.",
  "anya-e1004n3:106_0": "185.",
  "anya-e1004n3:107_0": "Saddhāpemesu santesu,",
  "anya-e1004n3:108_0": "Nagaṇe māsakaṁ sataṁ;",
  "anya-e1004n3:109_0": "Saddhāvemesva santesu,",
  "anya-e1004n3:110_0": "Māsakaṁpi sataṁ gaṇe.",
  "anya-e1004n3:111_0": "186.",
  "anya-e1004n3:112_0": "Adantadamanaṁ dānaṁ,",
  "anya-e1004n3:113_0": "Dānaṁ sabbatthasādhakaṁ;",
  "anya-e1004n3:114_0": "Dānena piyavācena,",
  "anya-e1004n3:115_0": "Unnamanti namantica.",
  "anya-e1004n3:116_0": "187.",
  "anya-e1004n3:117_0": "Dānaṁ sinehabhesajjaṁ,",
  "anya-e1004n3:118_0": "Maccheraṁ dussanosadhaṁ;",
  "anya-e1004n3:119_0": "Dānaṁ yasassabhosajjaṁ,",
  "anya-e1004n3:120_0": "Maccheraṁ kappanosadhaṁ.",
  "anya-e1004n3:121_0": "188.",
  "anya-e1004n3:122_0": "Dubbhikkhe annadātaṁca,",
  "anya-e1004n3:123_0": "Subhikkheca hiraññadaṁ;",
  "anya-e1004n3:124_0": "Bhayecābhayatādānaṁ,",
  "anya-e1004n3:125_0": "Saggepi bahu maññate.",
  "anya-e1004n3:126_0": "189.",
  "anya-e1004n3:127_0": "Sataṁcakkhu sataṁkaṇṇā,",
  "anya-e1004n3:128_0": "Nāyakassa sutā sadā;",
  "anya-e1004n3:129_0": "Tathāpi andhapadhīro;",
  "anya-e1004n3:130_0": "Esā nāyakadhammatā.",
  "anya-e1004n3:131_0": "190.",
  "anya-e1004n3:132_0": "Khamā jāgariyu ṭṭhānaṁ,",
  "anya-e1004n3:133_0": "Saṁvibhāgo dayikkhaṇā;",
  "anya-e1004n3:134_0": "Nāyakassa guṇāete,",
  "anya-e1004n3:135_0": "Icchitabbā hitatthinā.",
  "anya-e1004n3:136_0": "191.",
  "anya-e1004n3:137_0": "Paribhūto mudū hoti,",
  "anya-e1004n3:138_0": "Atitikkhoca veravā;",
  "anya-e1004n3:139_0": "Etañcaubhayaṁ ñatvā,",
  "anya-e1004n3:140_0": "Anumajjhaṁ samācare.",
  "anya-e1004n3:141_0": "192.",
  "anya-e1004n3:142_0": "Nekantamudunā sakkā,",
  "anya-e1004n3:143_0": "Ekantatikhiṇenavā;",
  "anya-e1004n3:144_0": "Attaṁ mahante ṭhapetuṁ,",
  "anya-e1004n3:145_0": "Tasmā ubhaya mācare.",
  "anya-e1004n3:146_0": "193.",
  "anya-e1004n3:147_0": "Piṭṭhito kkaṁ niseveyya,",
  "anya-e1004n3:148_0": "Kucchinātu hutāsanaṁ;",
  "anya-e1004n3:149_0": "Sāmikaṁ sabbabhāvena,",
  "anya-e1004n3:150_0": "Paralokaṁ amāyāya.",
  "anya-e1004n3:151_0": "194.",
  "anya-e1004n3:152_0": "Naseve pharusaṁsāmiṁ,",
  "anya-e1004n3:153_0": "Taṁpi sevena macchariṁ;",
  "anya-e1004n3:154_0": "Tato paggaṇhakaṁseve,",
  "anya-e1004n3:155_0": "Seve niggaṇhakaṁ tato;",
  "anya-e1004n3:156_0": "195.",
  "anya-e1004n3:157_0": "Na sā rājā yo ajeyyaṁ jināti,",
  "anya-e1004n3:158_0": "Na so sakhāyo sakhāraṁ jināti;",
  "anya-e1004n3:159_0": "Na sā bhariyā patino virodhati,",
  "anya-e1004n3:160_0": "Na te vuttā ye nabharanti jiṇṇakaṁ.",
  "anya-e1004n3:161_0": "196.",
  "anya-e1004n3:162_0": "Na sā sabhā yattha nasanti santo,",
  "anya-e1004n3:163_0": "Na te santo ye navadanti dhammaṁ;",
  "anya-e1004n3:164_0": "Rāgañca dosañca pahāya mohaṁ,",
  "anya-e1004n3:165_0": "Dhammaṁ bhaṇantāva bhavanti santo.",
  "anya-e1004n3:166_0": "197.",
  "anya-e1004n3:167_0": "Sutassa rakkhā satatābhiyogo;",
  "anya-e1004n3:168_0": "Kulassa vattaṁ purisassa vijjā;",
  "anya-e1004n3:169_0": "Rañño pamādo pasamodhanassa,",
  "anya-e1004n3:170_0": "Thīnantu jānāmi na jātu rakkhaṁ."
}