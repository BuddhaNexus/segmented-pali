{
  "anya-e0605n27:0_0": "Guṇadīpanīgāthā",
  "anya-e0605n27:1_0": "184.",
  "anya-e0605n27:2_0": "Na tassa adiṭṭhanamidhatthi kiñci,",
  "anya-e0605n27:3_0": "Ato aviññātamajānitabbaṁ;",
  "anya-e0605n27:4_0": "Sabbaṁ abhiññāsi yadatthi ñeyyaṁ,",
  "anya-e0605n27:5_0": "Tathāgato tena samantacakkhu.",
  "anya-e0605n27:6_0": "185.",
  "anya-e0605n27:7_0": "Iti mahitamanantākittisambhārasāraṁ,",
  "anya-e0605n27:8_0": "Sakaladasasahassīlokadhātumhi niccaṁ;",
  "anya-e0605n27:9_0": "Upacitasubhahetupayutānantakālaṁ,",
  "anya-e0605n27:10_0": "Tadiha sugatabodhisādhukaṁ cintanīyaṁ;",
  "anya-e0605n27:11_0": "186.",
  "anya-e0605n27:12_0": "Takkabyākaraṇañca dhammavinayaṁ sutvā pi yo paññavā,",
  "anya-e0605n27:13_0": "Tenāyaṁ sucisārabhūtavacanaṁ viññāyate kevalaṁ;",
  "anya-e0605n27:14_0": "Hetuñcāpi phalena tena saphalaṁ sampassamāno tato bodhiṁ saddahateva tassa mahatāvāyamato sambhavaṁ.",
  "anya-e0605n27:15_0": "187.",
  "anya-e0605n27:16_0": "Yo saddahanto pana tassa bodhiṁ,",
  "anya-e0605n27:17_0": "Vuttānusārena guṇerahādī;",
  "anya-e0605n27:18_0": "Katheti cintenti ca so muhuttaṁ,",
  "anya-e0605n27:19_0": "Ohāya pāpāni upeti santiṁ.",
  "anya-e0605n27:20_0": "188.",
  "anya-e0605n27:21_0": "Saddheyyā te cinteyyā te,",
  "anya-e0605n27:22_0": "Vandeyyā te pūjeyyāte;",
  "anya-e0605n27:23_0": "Buddholokāloke loke,",
  "anya-e0605n27:24_0": "Jāte netaṁ patthentena."
}