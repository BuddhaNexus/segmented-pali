{
  "tha-ap148:0.1_0": "Therāpadāna",
  "tha-ap148:0.2_0": "Chattavagga",
  "tha-ap148:0.3_0": "6 Anulepadāyakattheraapadāna",
  "tha-ap148:1.1_0": "“Anomadassīmunino,",
  "tha-ap148:1.2_0": "bodhivedimakāsahaṁ;",
  "tha-ap148:1.3_0": "Sudhāya piṇḍaṁ datvāna,",
  "tha-ap148:1.4_0": "pāṇikammaṁ akāsahaṁ.",
  "tha-ap148:2.1_0": "Disvā taṁ sukataṁ kammaṁ,",
  "tha-ap148:2.2_0": "anomadassī naruttamo;",
  "tha-ap148:2.3_0": "Bhikkhusaṅghe ṭhito satthā,",
  "tha-ap148:2.4_0": "imaṁ gāthaṁ abhāsatha.",
  "tha-ap148:3.1_0": "‘Iminā sudhakammena,",
  "tha-ap148:3.2_0": "cetanāpaṇidhīhi ca;",
  "tha-ap148:3.3_0": "Sampattiṁ anubhotvāna,",
  "tha-ap148:3.4_0": "dukkhassantaṁ karissati’.",
  "tha-ap148:4.1_0": "Pasannamukhavaṇṇomhi,",
  "tha-ap148:4.2_0": "ekaggo susamāhito;",
  "tha-ap148:4.3_0": "Dhāremi antimaṁ dehaṁ,",
  "tha-ap148:4.4_0": "sammāsambuddhasāsane.",
  "tha-ap148:5.1_0": "Ito kappasate āsiṁ,",
  "tha-ap148:5.2_0": "paripuṇṇe anūnake;",
  "tha-ap148:5.3_0": "Rājā sabbaghano nāma,",
  "tha-ap148:5.4_0": "cakkavattī mahabbalo.",
  "tha-ap148:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap148:6.2_0": "…pe…",
  "tha-ap148:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap148:7.1_0": "Itthaṁ sudaṁ āyasmā anulepadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap148:8.1_0": "Anulepadāyakattherassāpadānaṁ chaṭṭhaṁ."
}