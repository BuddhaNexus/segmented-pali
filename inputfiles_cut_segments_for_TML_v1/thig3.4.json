{
  "thig3.4:0.1_0": "Therīgāthā",
  "thig3.4:0.2_0": "Tikanipāta",
  "thig3.4:0.3_0": "4. Dantikātherīgāthā",
  "thig3.4:1.1_0": "“Divāvihārā nikkhamma,",
  "thig3.4:1.2_0": "gijjhakūṭamhi pabbate;",
  "thig3.4:1.3_0": "Nāgaṁ ogāhamuttiṇṇaṁ,",
  "thig3.4:1.4_0": "nadītīramhi addasaṁ.",
  "thig3.4:2.1_0": "Puriso aṅkusamādāya,",
  "thig3.4:2.2_0": "‘dehi pādan’ti yācati;",
  "thig3.4:2.3_0": "Nāgo pasārayī pādaṁ,",
  "thig3.4:2.4_0": "puriso nāgamāruhi.",
  "thig3.4:3.1_0": "Disvā adantaṁ damitaṁ,",
  "thig3.4:3.2_0": "manussānaṁ vasaṁ gataṁ;",
  "thig3.4:3.3_0": "Tato cittaṁ samādhesiṁ,",
  "thig3.4:3.4_0": "khalu tāya vanaṁ gatā”ti.",
  "thig3.4:4.1_0": "… Dantikā therī …."
}