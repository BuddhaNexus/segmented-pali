{
  "tha-ap456:0.1_0": "Therāpadāna",
  "tha-ap456:0.2_0": "Jagatidāyakavagga",
  "tha-ap456:0.3_0": "4 Tiṇukkadhāriyattheraapadāna",
  "tha-ap456:1.1_0": "“Padumuttarabuddhassa,",
  "tha-ap456:1.2_0": "bodhiyā pādaputtame;",
  "tha-ap456:1.3_0": "Pasannacitto sumano,",
  "tha-ap456:1.4_0": "tayo ukke adhārayiṁ.",
  "tha-ap456:2.1_0": "Satasahassito kappe,",
  "tha-ap456:2.2_0": "sohaṁ ukkamadhārayiṁ;",
  "tha-ap456:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap456:2.4_0": "ukkadānassidaṁ phalaṁ.",
  "tha-ap456:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap456:3.2_0": "…pe…",
  "tha-ap456:3.3_0": "viharāmi anāsavo.",
  "tha-ap456:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap456:4.2_0": "…pe…",
  "tha-ap456:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap456:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap456:5.2_0": "…pe…",
  "tha-ap456:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap456:6.1_0": "Itthaṁ sudaṁ āyasmā tiṇukkadhāriyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap456:7.1_0": "Tiṇukkadhāriyattherassāpadānaṁ catutthaṁ."
}