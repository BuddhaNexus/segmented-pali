{
  "ja434:0.1_0": "Jātaka",
  "ja434:0.2_0": "Navakanipāta",
  "ja434:0.3_0": "Gijjhavagga",
  "ja434:0.4_0": "8. Cakkavākajātaka",
  "ja434:1.1_0": "“Kāsāyavatthe sakuṇe vadāmi,",
  "ja434:1.2_0": "Duve duve nandamane carante;",
  "ja434:1.3_0": "Kaṁ aṇḍajaṁ aṇḍajā mānusesu,",
  "ja434:1.4_0": "Jātiṁ pasaṁsanti tadiṅgha brūtha”.",
  "ja434:2.1_0": "“Amhe manussesu manussahiṁsa,",
  "ja434:2.2_0": "Anubbate cakkavāke vadanti;",
  "ja434:2.3_0": "Kalyāṇabhāvamhe dijesu sammatā,",
  "ja434:2.4_0": "Abhirūpā vicarāma aṇṇave”.",
  "ja434:2.5_0": "(…)",
  "ja434:3.1_0": "“Kiṁ aṇṇave kāni phalāni bhuñje,",
  "ja434:3.2_0": "Maṁsaṁ kuto khādatha cakkavākā;",
  "ja434:3.3_0": "Kiṁ bhojanaṁ bhuñjatha vo anomā,",
  "ja434:3.4_0": "Balañca vaṇṇo ca anapparūpā”.",
  "ja434:4.1_0": "“Na aṇṇave santi phalāni dhaṅka,",
  "ja434:4.2_0": "Maṁsaṁ kuto khādituṁ cakkavāke;",
  "ja434:4.3_0": "Sevālabhakkhamha apāṇabhojanā,",
  "ja434:4.4_0": "Na ghāsahetūpi karoma pāpaṁ”.",
  "ja434:5.1_0": "“Na me idaṁ ruccati cakkavāka,",
  "ja434:5.2_0": "Asmiṁ bhave bhojanasannikāso;",
  "ja434:5.3_0": "Ahosi pubbe tato me aññathā,",
  "ja434:5.4_0": "Icceva me vimati ettha jātā.",
  "ja434:6.1_0": "Ahampi maṁsāni phalāni bhuñje,",
  "ja434:6.2_0": "Annāni ca loṇiyateliyāni;",
  "ja434:6.3_0": "Rasaṁ manussesu labhāmi bhottuṁ,",
  "ja434:6.4_0": "Sūrova saṅgāmamukhaṁ vijetvā;",
  "ja434:6.5_0": "Na ca me tādiso vaṇṇo,",
  "ja434:6.6_0": "Cakkavāka yathā tava”.",
  "ja434:7.1_0": "“Asuddhabhakkhosi khaṇānupātī,",
  "ja434:7.2_0": "Kicchena te labbhati annapānaṁ;",
  "ja434:7.3_0": "Na tussasī rukkhaphalehi dhaṅka,",
  "ja434:7.4_0": "Maṁsāni vā yāni susānamajjhe.",
  "ja434:8.1_0": "Yo sāhasena adhigamma bhoge,",
  "ja434:8.2_0": "Paribhuñjati dhaṅka khaṇānupātī;",
  "ja434:8.3_0": "Tato upakkosati naṁ sabhāvo,",
  "ja434:8.4_0": "Upakkuṭṭho vaṇṇabalaṁ jahāti.",
  "ja434:9.1_0": "Appampi ce nibbutiṁ bhuñjatī yadi,",
  "ja434:9.2_0": "Asāhasena aparūpaghātī;",
  "ja434:9.3_0": "Balañca vaṇṇo ca tadassa hoti,",
  "ja434:9.4_0": "Na hi sabbo āhāramayena vaṇṇo”ti.",
  "ja434:10.1_0": "Cakkavākajātakaṁ aṭṭhamaṁ."
}