{
  "ja95:0.1_0": "Jātaka",
  "ja95:0.2_0": "Ekakanipāta",
  "ja95:0.3_0": "Littavagga",
  "ja95:0.4_0": "5. Mahāsudassanajātaka",
  "ja95:1.1_0": "“Aniccā vata saṅkhārā,",
  "ja95:1.2_0": "uppādavayadhammino;",
  "ja95:1.3_0": "Uppajjitvā nirujjhanti,",
  "ja95:1.4_0": "tesaṁ vūpasamo sukho”ti.",
  "ja95:2.1_0": "Mahāsudassanajātakaṁ pañcamaṁ."
}