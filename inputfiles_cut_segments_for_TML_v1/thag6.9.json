{
  "thag6.9:0.1_0": "Theragāthā",
  "thag6.9:0.2_0": "Chakkanipāta",
  "thag6.9:0.3_0": "Paṭhamavagga",
  "thag6.9:0.4_0": "6.9 Purohitaputtajentattheragāthā",
  "thag6.9:1.1_0": "“Jātimadena mattohaṁ,",
  "thag6.9:1.2_0": "bhogaissariyena ca;",
  "thag6.9:1.3_0": "Saṇṭhānavaṇṇarūpena,",
  "thag6.9:1.4_0": "madamatto acārihaṁ.",
  "thag6.9:2.1_0": "Nāttano samakaṁ kañci,",
  "thag6.9:2.2_0": "atirekaṁ ca maññisaṁ;",
  "thag6.9:2.3_0": "Atimānahato bālo,",
  "thag6.9:2.4_0": "patthaddho ussitaddhajo.",
  "thag6.9:3.1_0": "Mātaraṁ pitarañcāpi,",
  "thag6.9:3.2_0": "aññepi garusammate;",
  "thag6.9:3.3_0": "Na kañci abhivādesiṁ,",
  "thag6.9:3.4_0": "mānatthaddho anādaro.",
  "thag6.9:4.1_0": "Disvā vināyakaṁ aggaṁ,",
  "thag6.9:4.2_0": "sārathīnaṁ varuttamaṁ;",
  "thag6.9:4.3_0": "Tapantamiva ādiccaṁ,",
  "thag6.9:4.4_0": "bhikkhusaṅghapurakkhataṁ.",
  "thag6.9:5.1_0": "Mānaṁ madañca chaḍḍetvā,",
  "thag6.9:5.2_0": "vippasannena cetasā;",
  "thag6.9:5.3_0": "Sirasā abhivādesiṁ,",
  "thag6.9:5.4_0": "sabbasattānamuttamaṁ.",
  "thag6.9:6.1_0": "Atimāno ca omāno,",
  "thag6.9:6.2_0": "pahīnā susamūhatā;",
  "thag6.9:6.3_0": "Asmimāno samucchinno,",
  "thag6.9:6.4_0": "sabbe mānavidhā hatā”ti.",
  "thag6.9:6.5_0": "… Jento purohitaputto thero …."
}