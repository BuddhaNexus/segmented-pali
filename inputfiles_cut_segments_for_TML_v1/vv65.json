{
  "vv65:0.1_0": "Vimānavatthu",
  "vv65:0.2_0": "Purisavimāna",
  "vv65:0.3_0": "Pāyāsivagga",
  "vv65:0.4_0": "1. Paṭhamaagāriyavimānavatthu",
  "vv65:1.1_0": "“Yathā vanaṁ cittalataṁ pabhāsati,",
  "vv65:1.2_0": "Uyyānaseṭṭhaṁ tidasānamuttamaṁ;",
  "vv65:1.3_0": "Tathūpamaṁ tuyhamidaṁ vimānaṁ,",
  "vv65:1.4_0": "Obhāsayaṁ tiṭṭhati antalikkhe.",
  "vv65:2.1_0": "Deviddhipattosi mahānubhāvo,",
  "vv65:2.2_0": "Manussabhūto kimakāsi puññaṁ;",
  "vv65:2.3_0": "Kenāsi evaṁ jalitānubhāvo,",
  "vv65:2.4_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv65:3.1_0": "So devaputto attamano,",
  "vv65:3.2_0": "moggallānena pucchito;",
  "vv65:3.3_0": "Pañhaṁ puṭṭho viyākāsi,",
  "vv65:3.4_0": "yassa kammassidaṁ phalaṁ.",
  "vv65:4.1_0": "“Ahañca bhariyā ca manussaloke,",
  "vv65:4.2_0": "Opānabhūtā gharamāvasimha;",
  "vv65:4.3_0": "Annañca pānañca pasannacittā,",
  "vv65:4.4_0": "Sakkacca dānaṁ vipulaṁ adamha.",
  "vv65:5.1_0": "Tena metādiso vaṇṇo,",
  "vv65:5.2_0": "…pe…",
  "vv65:5.3_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv65:6.1_0": "Paṭhamaagāriyavimānaṁ paṭhamaṁ."
}