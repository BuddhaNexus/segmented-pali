{
  "tha-ap292:0.1_0": "Therāpadāna",
  "tha-ap292:0.2_0": "Paṇṇadāyakavagga",
  "tha-ap292:0.3_0": "10 Pubbaṅgamiyattheraapadāna",
  "tha-ap292:1.1_0": "“Cullāsītisahassāni,",
  "tha-ap292:1.2_0": "pabbajimha akiñcanā;",
  "tha-ap292:1.3_0": "Tesaṁ pubbaṅgamo āsiṁ,",
  "tha-ap292:1.4_0": "uttamatthassa pattiyā.",
  "tha-ap292:2.1_0": "Sarāgā sabhavā cete,",
  "tha-ap292:2.2_0": "vippasannamanāvilā;",
  "tha-ap292:2.3_0": "Upaṭṭhahiṁsu sakkaccaṁ,",
  "tha-ap292:2.4_0": "pasannā sehi pāṇibhi.",
  "tha-ap292:3.1_0": "Khīṇāsavā vantadosā,",
  "tha-ap292:3.2_0": "katakiccā anāsavā;",
  "tha-ap292:3.3_0": "Phariṁsu mettacittena,",
  "tha-ap292:3.4_0": "sayambhū aparājitā.",
  "tha-ap292:4.1_0": "Tesaṁ upaṭṭhahitvāna,",
  "tha-ap292:4.2_0": "sambuddhānaṁ patissato;",
  "tha-ap292:4.3_0": "Maraṇañca anuppatto,",
  "tha-ap292:4.4_0": "devattañca agamhase.",
  "tha-ap292:5.1_0": "Catunnavutito kappe,",
  "tha-ap292:5.2_0": "yaṁ sīlamanupālayiṁ;",
  "tha-ap292:5.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap292:5.4_0": "saññamassa idaṁ phalaṁ.",
  "tha-ap292:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap292:6.2_0": "…pe…",
  "tha-ap292:6.3_0": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap292:7.1_0": "Itthaṁ sudaṁ āyasmā pubbaṅgamiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap292:8.1_0": "Pubbaṅgamiyattherassāpadānaṁ dasamaṁ.",
  "tha-ap292:9.1_0": "Paṇṇadāyakavaggo ekūnatiṁsatimo.",
  "tha-ap292:10.1_0": "Tassuddānaṁ",
  "tha-ap292:11.1_0": "Paṇṇaṁ phalaṁ paccuggamaṁ,",
  "tha-ap292:11.2_0": "ekapupphi ca maghavā;",
  "tha-ap292:11.3_0": "Upaṭṭhākāpadānañca,",
  "tha-ap292:11.4_0": "pabbajjā buddhupaṭṭhāko;",
  "tha-ap292:11.5_0": "Pubbaṅgamo ca gāthāyo,",
  "tha-ap292:11.6_0": "aṭṭhatālīsa kittitā."
}