{
  "ja277:0.1_0": "Jātaka",
  "ja277:0.2_0": "Tikanipāta",
  "ja277:0.3_0": "Udapānavagga",
  "ja277:0.4_0": "7. Romakajātaka",
  "ja277:1.1_0": "“Vassāni paññāsa samādhikāni,",
  "ja277:1.2_0": "Vasimha selassa guhāya romaka;",
  "ja277:1.3_0": "Asaṅkamānā abhinibbutattā,",
  "ja277:1.4_0": "Hatthattamāyanti mamaṇḍajā pure.",
  "ja277:2.1_0": "Te dāni vakkaṅga kimatthamussukā,",
  "ja277:2.2_0": "Bhajanti aññaṁ girikandaraṁ dijā;",
  "ja277:2.3_0": "Na nūna maññanti mamaṁ yathā pure,",
  "ja277:2.4_0": "Cirappavutthā atha vā na te ime”.",
  "ja277:3.1_0": "“Jānāma taṁ na mayaṁ sampamūḷhā,",
  "ja277:3.2_0": "Soyeva tvaṁ te mayamasma nāññe;",
  "ja277:3.3_0": "Cittañca te asmiṁ jane paduṭṭhaṁ,",
  "ja277:3.4_0": "Ājīvikā tena tamuttasāmā”ti.",
  "ja277:4.1_0": "Romakajātakaṁ sattamaṁ."
}