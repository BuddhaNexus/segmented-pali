{
  "ja16:0.1_0": "Jātaka",
  "ja16:0.2_0": "Ekakanipāta",
  "ja16:0.3_0": "Sīlavagga",
  "ja16:0.4_0": "6. Tipallatthamigajātaka",
  "ja16:1.1_0": "“Migaṁ tipallatthamanekamāyaṁ,",
  "ja16:1.2_0": "Aṭṭhakkhuraṁ aḍḍharattāpapāyiṁ;",
  "ja16:1.3_0": "Ekena sotena chamāssasanto,",
  "ja16:1.4_0": "Chahi kalāhitibhoti bhāgineyyo”ti.",
  "ja16:2.1_0": "Tipallatthamigajātakaṁ chaṭṭhaṁ."
}