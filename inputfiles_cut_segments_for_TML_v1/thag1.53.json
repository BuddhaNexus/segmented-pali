{
  "thag1.53:0.1_0": "Theragāthā",
  "thag1.53:0.2_0": "Ekakanipāta",
  "thag1.53:0.3_0": "Chaṭṭhavagga",
  "thag1.53:0.4_0": "1.53. Valliyattheragāthā",
  "thag1.53:1.1_0": "“Vassati devo yathā sugītaṁ,",
  "thag1.53:1.2_0": "Channā me kuṭikā sukhā nivātā;",
  "thag1.53:1.3_0": "Tassaṁ viharāmi appamatto,",
  "thag1.53:1.4_0": "Atha ce patthayasi pavassa devā”ti.",
  "thag1.53:1.5_0": "… Valliyo thero …."
}