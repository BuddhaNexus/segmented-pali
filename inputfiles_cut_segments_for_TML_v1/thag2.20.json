{
  "thag2.20:0.1_0": "Theragāthā",
  "thag2.20:0.2_0": "Dukanipāta",
  "thag2.20:0.3_0": "Dutiyavagga",
  "thag2.20:0.4_0": "2.20. Sirimattheragāthā",
  "thag2.20:1.1_0": "“Pare ca naṁ pasaṁsanti,",
  "thag2.20:1.2_0": "attā ce asamāhito;",
  "thag2.20:1.3_0": "Moghaṁ pare pasaṁsanti,",
  "thag2.20:1.4_0": "attā hi asamāhito.",
  "thag2.20:2.1_0": "Pare ca naṁ garahanti,",
  "thag2.20:2.2_0": "attā ce susamāhito;",
  "thag2.20:2.3_0": "Moghaṁ pare garahanti,",
  "thag2.20:2.4_0": "attā hi susamāhito”ti.",
  "thag2.20:2.5_0": "…",
  "thag2.20:2.6_0": "Sirimā thero ….",
  "thag2.20:2.7_0": "vaggo dutiyo.",
  "thag2.20:3.0_0": "Tassuddānaṁ",
  "thag2.20:3.1_0": "Cundo ca jotidāso ca,",
  "thag2.20:3.2_0": "thero heraññakāni ca;",
  "thag2.20:3.3_0": "Somamitto sabbamitto,",
  "thag2.20:3.4_0": "kāḷo tisso ca kimilo;",
  "thag2.20:3.5_0": "Nando ca sirimā ceva,",
  "thag2.20:3.6_0": "dasa therā mahiddhikāti."
}