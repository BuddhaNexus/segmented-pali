{
  "kv13.4:0.1_0": "Kathāvatthu",
  "kv13.4:0.2_0": "Mahāpaṇṇāsaka",
  "kv13.4:0.3_0": "Terasamavagga",
  "kv13.4:0.4_0": "Niyatassaniyāmakathā",
  "kv13.4:1.1_0": "Niyato niyāmaṁ okkamatīti?",
  "kv13.4:1.2_0": "Āmantā.",
  "kv13.4:1.3_0": "Micchattaniyato sammattaniyāmaṁ okkamati, sammattaniyato micchattaniyāmaṁ okkamatīti?",
  "kv13.4:1.4_0": "Na hevaṁ vattabbe …pe….",
  "kv13.4:2.1_0": "Niyato niyāmaṁ okkamatīti?",
  "kv13.4:2.2_0": "Āmantā.",
  "kv13.4:2.3_0": "Pubbe maggaṁ bhāvetvā pacchā niyāmaṁ okkamatīti?",
  "kv13.4:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv13.4:2.5_0": "pubbe sotāpattimaggaṁ bhāvetvā pacchā sotāpattiniyāmaṁ okkamatīti?",
  "kv13.4:2.6_0": "Na hevaṁ vattabbe …pe…",
  "kv13.4:2.7_0": "pubbe sakadāgāmi …pe…",
  "kv13.4:2.8_0": "anāgāmi …pe…",
  "kv13.4:2.9_0": "arahattamaggaṁ bhāvetvā pacchā arahattaniyāmaṁ okkamatīti?",
  "kv13.4:2.10_0": "Na hevaṁ vattabbe …pe….",
  "kv13.4:3.1_0": "Pubbe satipaṭṭhānaṁ …pe…",
  "kv13.4:3.2_0": "sammappadhānaṁ … iddhipādaṁ … indriyaṁ … balaṁ … bojjhaṅgaṁ bhāvetvā pacchā niyāmaṁ okkamatīti?",
  "kv13.4:3.3_0": "Na hevaṁ vattabbe …pe….",
  "kv13.4:4.1_0": "Na vattabbaṁ—",
  "kv13.4:4.2_0": "“niyato niyāmaṁ okkamatī”ti?",
  "kv13.4:4.3_0": "Āmantā.",
  "kv13.4:4.4_0": "Bhabbo bodhisatto tāya jātiyā dhammaṁ nābhisametunti?",
  "kv13.4:4.5_0": "Na hevaṁ vattabbe.",
  "kv13.4:4.6_0": "Tena hi niyato niyāmaṁ okkamatīti.",
  "kv13.4:4.7_0": "Niyatassa niyāmakathā niṭṭhitā."
}