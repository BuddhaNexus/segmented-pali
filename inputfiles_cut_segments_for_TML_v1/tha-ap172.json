{
  "tha-ap172:0.1_0": "Therāpadāna",
  "tha-ap172:0.2_0": "Supāricariyavagga",
  "tha-ap172:0.3_0": "10. Pañcaṅguliyattheraapadāna",
  "tha-ap172:1.1_0": "“Tisso nāmāsi bhagavā,",
  "tha-ap172:1.2_0": "lokajeṭṭho narāsabho;",
  "tha-ap172:1.3_0": "Pavisati gandhakuṭiṁ,",
  "tha-ap172:1.4_0": "vihārakusalo muni.",
  "tha-ap172:2.1_0": "Sugandhamālamādāya,",
  "tha-ap172:2.2_0": "agamāsiṁ jinantikaṁ;",
  "tha-ap172:2.3_0": "Apasaddo ca sambuddhe,",
  "tha-ap172:2.4_0": "pañcaṅgulimadāsahaṁ.",
  "tha-ap172:3.1_0": "Dvenavute ito kappe,",
  "tha-ap172:3.2_0": "yaṁ gandhamabhiropayiṁ;",
  "tha-ap172:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap172:3.4_0": "pañcaṅgulissidaṁ phalaṁ.",
  "tha-ap172:4.1_0": "Dvesattatimhito kappe,",
  "tha-ap172:4.2_0": "rājā āsiṁ sayampabho;",
  "tha-ap172:4.3_0": "Sattaratanasampanno,",
  "tha-ap172:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap172:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap172:5.2_0": "…pe…",
  "tha-ap172:5.3_0": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap172:6.1_0": "Itthaṁ sudaṁ āyasmā pañcaṅguliyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap172:7.1_0": "Pañcaṅguliyattherassāpadānaṁ dasamaṁ.",
  "tha-ap172:8.1_0": "Supāricariyavaggo sattarasamo.",
  "tha-ap172:9.1_0": "Tassuddānaṁ",
  "tha-ap172:10.1_0": "Supāricari kaṇaverī,",
  "tha-ap172:10.2_0": "khajjako desapūjako;",
  "tha-ap172:10.3_0": "Kaṇikāro sappidado,",
  "tha-ap172:10.4_0": "yūthiko dussadāyako;",
  "tha-ap172:10.5_0": "Māḷo ca pañcaṅguliko,",
  "tha-ap172:10.6_0": "catupaññāsa gāthakāti."
}