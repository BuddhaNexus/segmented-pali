{
  "tha-ap158:0.1_0": "Therāpadāna",
  "tha-ap158:0.2_0": "Bandhujīvakavagga",
  "tha-ap158:0.3_0": "6. Kadambapupphiyattheraapadāna",
  "tha-ap158:1.1_0": "“Himavantassāvidūre,",
  "tha-ap158:1.2_0": "kukkuṭo nāma pabbato;",
  "tha-ap158:1.3_0": "Tamhi pabbatapādamhi,",
  "tha-ap158:1.4_0": "satta buddhā vasanti te.",
  "tha-ap158:2.1_0": "Kadambaṁ pupphitaṁ disvā,",
  "tha-ap158:2.2_0": "dīparājaṁva uggataṁ;",
  "tha-ap158:2.3_0": "Ubho hatthehi paggayha,",
  "tha-ap158:2.4_0": "satta buddhe samokiriṁ.",
  "tha-ap158:3.1_0": "Catunnavutito kappe,",
  "tha-ap158:3.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap158:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap158:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap158:4.1_0": "Dvenavute ito kappe,",
  "tha-ap158:4.2_0": "sattāsuṁ pupphanāmakā;",
  "tha-ap158:4.3_0": "Sattaratanasampannā,",
  "tha-ap158:4.4_0": "cakkavattī mahabbalā.",
  "tha-ap158:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap158:5.2_0": "…pe…",
  "tha-ap158:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap158:6.1_0": "Itthaṁ sudaṁ āyasmā kadambapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap158:7.1_0": "Kadambapupphiyattherassāpadānaṁ chaṭṭhaṁ."
}