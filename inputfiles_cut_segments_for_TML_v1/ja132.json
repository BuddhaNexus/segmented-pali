{
  "ja132:0.1_0": "Jātaka",
  "ja132:0.2_0": "Ekakanipāta",
  "ja132:0.3_0": "Asampadānavagga",
  "ja132:0.4_0": "2. Bhīrukajātaka",
  "ja132:1.1_0": "“Kusalūpadese dhitiyā daḷhāya ca,",
  "ja132:1.2_0": "Anivattitattābhayabhīrutāya ca;",
  "ja132:1.3_0": "Na rakkhasīnaṁ vasamāgamimhase,",
  "ja132:1.4_0": "Sa sotthibhāvo mahatā bhayena me”ti.",
  "ja132:2.1_0": "Bhīrukajātakaṁ dutiyaṁ."
}