{
  "tha-ap372:0.1_0": "Therāpadāna",
  "tha-ap372:0.2_0": "Mandāravapupphiyavagga",
  "tha-ap372:0.3_0": "10 Saḷalapupphiyattheraapadāna",
  "tha-ap372:1.1_0": "“Candabhāgānadītīre,",
  "tha-ap372:1.2_0": "ahosiṁ kinnaro tadā;",
  "tha-ap372:1.3_0": "Tatthaddasaṁ devadevaṁ,",
  "tha-ap372:1.4_0": "caṅkamantaṁ narāsabhaṁ.",
  "tha-ap372:2.1_0": "Ocinitvāna saḷalaṁ,",
  "tha-ap372:2.2_0": "pupphaṁ buddhassadāsahaṁ;",
  "tha-ap372:2.3_0": "Upasiṅghi mahāvīro,",
  "tha-ap372:2.4_0": "saḷalaṁ devagandhikaṁ.",
  "tha-ap372:3.1_0": "Paṭiggahetvā sambuddho,",
  "tha-ap372:3.2_0": "vipassī lokanāyako;",
  "tha-ap372:3.3_0": "Upasiṅghi mahāvīro,",
  "tha-ap372:3.4_0": "pekkhamānassa me sato.",
  "tha-ap372:4.1_0": "Pasannacitto sumano,",
  "tha-ap372:4.2_0": "vanditvā dvipaduttamaṁ;",
  "tha-ap372:4.3_0": "Añjaliṁ paggahetvāna,",
  "tha-ap372:4.4_0": "puna pabbatamāruhiṁ.",
  "tha-ap372:5.1_0": "Ekanavutito kappe,",
  "tha-ap372:5.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap372:5.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap372:5.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap372:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap372:6.2_0": "…pe…",
  "tha-ap372:6.3_0": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap372:7.1_0": "Itthaṁ sudaṁ āyasmā saḷalapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap372:8.1_0": "Saḷalapupphiyattherassāpadānaṁ dasamaṁ.",
  "tha-ap372:9.1_0": "Mandāravapupphiyavaggo sattatiṁsatimo.",
  "tha-ap372:10.1_0": "Tassuddānaṁ",
  "tha-ap372:11.1_0": "Mandāravañca kakkāru,",
  "tha-ap372:11.2_0": "bhisakesarapupphiyo;",
  "tha-ap372:11.3_0": "Aṅkolako kadambī ca,",
  "tha-ap372:11.4_0": "uddālī ekacampako;",
  "tha-ap372:11.5_0": "Timiraṁ saḷalañceva,",
  "tha-ap372:11.6_0": "gāthā tālīsameva ca."
}