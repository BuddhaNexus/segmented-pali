{
  "pv48:0.1_0": "Petavatthu",
  "pv48:0.2_0": "Mahāvagga",
  "pv48:0.3_0": "13. Akkharukkhapetavatthu",
  "pv48:1.1_0": "“Yaṁ dadāti na taṁ hoti,",
  "pv48:1.2_0": "Detheva dānaṁ datvā ubhayaṁ tarati;",
  "pv48:1.3_0": "Ubhayaṁ tena dānena gacchati,",
  "pv48:1.4_0": "Jāgaratha māpamajjathā”ti.",
  "pv48:2.1_0": "Akkharukkhapetavatthu terasamaṁ."
}