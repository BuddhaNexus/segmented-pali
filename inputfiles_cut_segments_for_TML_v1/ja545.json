{
  "ja545:0.1_0": "Jātaka",
  "ja545:0.2_0": "Mahānipāta",
  "ja545:0.3_0": "Mūgapakkhavagga",
  "ja545:0.4_0": "8. Mahānāradakassapajātaka",
  "ja545:1.1_0": "Ahu rājā videhānaṁ,",
  "ja545:1.2_0": "aṅgati nāma khattiyo;",
  "ja545:1.3_0": "Pahūtayoggo dhanimā,",
  "ja545:1.4_0": "anantabalaporiso.",
  "ja545:2.1_0": "So ca pannarasiṁ rattiṁ,",
  "ja545:2.2_0": "purimayāme anāgate;",
  "ja545:2.3_0": "Cātumāsā komudiyā,",
  "ja545:2.4_0": "amacce sannipātayi.",
  "ja545:3.1_0": "Paṇḍite sutasampanne,",
  "ja545:3.2_0": "mhitapubbe vicakkhaṇe;",
  "ja545:3.3_0": "Vijayañca sunāmañca,",
  "ja545:3.4_0": "senāpatiṁ alātakaṁ.",
  "ja545:4.1_0": "Tamanupucchi vedeho,",
  "ja545:4.2_0": "“paccekaṁ brūtha saṁ ruciṁ;",
  "ja545:4.3_0": "Cātumāsā komudajja,",
  "ja545:4.4_0": "juṇhaṁ byapahataṁ tamaṁ;",
  "ja545:4.5_0": "Kāyajja ratiyā rattiṁ,",
  "ja545:4.6_0": "viharemu imaṁ utuṁ”.",
  "ja545:5.1_0": "Tato senāpati rañño,",
  "ja545:5.2_0": "Alāto etadabravi;",
  "ja545:5.3_0": "“Haṭṭhaṁ yoggaṁ balaṁ sabbaṁ,",
  "ja545:5.4_0": "Senaṁ sannāhayāmase.",
  "ja545:6.1_0": "Niyyāma deva yuddhāya,",
  "ja545:6.2_0": "anantabalaporisā;",
  "ja545:6.3_0": "Ye te vasaṁ na āyanti,",
  "ja545:6.4_0": "vasaṁ upanayāmase;",
  "ja545:6.5_0": "Esā mayhaṁ sakā diṭṭhi,",
  "ja545:6.6_0": "ajitaṁ ojināmase”.",
  "ja545:7.1_0": "Alātassa vaco sutvā,",
  "ja545:7.2_0": "sunāmo etadabravi;",
  "ja545:7.3_0": "“Sabbe tuyhaṁ mahārāja,",
  "ja545:7.4_0": "amittā vasamāgatā.",
  "ja545:8.1_0": "Nikkhittasatthā paccatthā,",
  "ja545:8.2_0": "nivātamanuvattare;",
  "ja545:8.3_0": "Uttamo ussavo ajja,",
  "ja545:8.4_0": "na yuddhaṁ mama ruccati.",
  "ja545:9.1_0": "Annapānañca khajjañca,",
  "ja545:9.2_0": "khippaṁ abhiharantu te;",
  "ja545:9.3_0": "Ramassu deva kāmehi,",
  "ja545:9.4_0": "naccagīte suvādite”.",
  "ja545:10.1_0": "Sunāmassa vaco sutvā,",
  "ja545:10.2_0": "vijayo etadabravi;",
  "ja545:10.3_0": "“Sabbe kāmā mahārāja,",
  "ja545:10.4_0": "niccaṁ tava mupaṭṭhitā.",
  "ja545:11.1_0": "Na hete dullabhā deva,",
  "ja545:11.2_0": "tava kāmehi modituṁ;",
  "ja545:11.3_0": "Sadāpi kāmā sulabhā,",
  "ja545:11.4_0": "netaṁ cittamataṁ mama.",
  "ja545:12.1_0": "Samaṇaṁ brāhmaṇaṁ vāpi,",
  "ja545:12.2_0": "upāsemu bahussutaṁ;",
  "ja545:12.3_0": "Yo najja vinaye kaṅkhaṁ,",
  "ja545:12.4_0": "atthadhammavidū ise”.",
  "ja545:13.1_0": "Vijayassa vaco sutvā,",
  "ja545:13.2_0": "rājā aṅgati mabravi;",
  "ja545:13.3_0": "“Yathā vijayo bhaṇati,",
  "ja545:13.4_0": "mayhampetaṁva ruccati.",
  "ja545:14.1_0": "Samaṇaṁ brāhmaṇaṁ vāpi,",
  "ja545:14.2_0": "upāsemu bahussutaṁ;",
  "ja545:14.3_0": "Yo najja vinaye kaṅkhaṁ,",
  "ja545:14.4_0": "atthadhammavidū ise.",
  "ja545:15.1_0": "Sabbeva santā karotha matiṁ,",
  "ja545:15.2_0": "Kaṁ upāsemu paṇḍitaṁ;",
  "ja545:15.3_0": "Yo najja vinaye kaṅkhaṁ,",
  "ja545:15.4_0": "Atthadhammavidū ise”.",
  "ja545:16.1_0": "Vedehassa vaco sutvā,",
  "ja545:16.2_0": "alāto etadabravi;",
  "ja545:16.3_0": "“Atthāyaṁ migadāyasmiṁ,",
  "ja545:16.4_0": "acelo dhīrasammato.",
  "ja545:17.1_0": "Guṇo kassapagottāyaṁ,",
  "ja545:17.2_0": "suto citrakathī gaṇī;",
  "ja545:17.3_0": "Taṁ deva payirupāsemu,",
  "ja545:17.4_0": "so no kaṅkhaṁ vinessati”.",
  "ja545:18.1_0": "Alātassa vaco sutvā,",
  "ja545:18.2_0": "rājā codesi sārathiṁ;",
  "ja545:18.3_0": "“Migadāyaṁ gamissāma,",
  "ja545:18.4_0": "yuttaṁ yānaṁ idhānaya”.",
  "ja545:19.1_0": "Tassa yānaṁ ayojesuṁ,",
  "ja545:19.2_0": "dantaṁ rūpiyapakkharaṁ;",
  "ja545:19.3_0": "Sukkamaṭṭhaparivāraṁ,",
  "ja545:19.4_0": "paṇḍaraṁ dosināmukhaṁ.",
  "ja545:20.1_0": "Tatrāsuṁ kumudāyuttā,",
  "ja545:20.2_0": "cattāro sindhavā hayā;",
  "ja545:20.3_0": "Anilūpamasamuppātā,",
  "ja545:20.4_0": "sudantā soṇṇamālino.",
  "ja545:21.1_0": "Setacchattaṁ setaratho,",
  "ja545:21.2_0": "setassā setabījanī;",
  "ja545:21.3_0": "Vedeho sahamaccehi,",
  "ja545:21.4_0": "niyyaṁ candova sobhati.",
  "ja545:22.1_0": "Tamanuyāyiṁsu bahavo,",
  "ja545:22.2_0": "indikhaggadharā balī;",
  "ja545:22.3_0": "Assapiṭṭhigatā vīrā,",
  "ja545:22.4_0": "narā naravarādhipaṁ.",
  "ja545:23.1_0": "So muhuttaṁva yāyitvā,",
  "ja545:23.2_0": "yānā oruyha khattiyo;",
  "ja545:23.3_0": "Vedeho sahamaccehi,",
  "ja545:23.4_0": "pattī guṇamupāgami.",
  "ja545:24.1_0": "Yepi tattha tadā āsuṁ,",
  "ja545:24.2_0": "brāhmaṇibbhā samāgatā;",
  "ja545:24.3_0": "Na te apanayī rājā,",
  "ja545:24.4_0": "akataṁ bhūmimāgate.",
  "ja545:25.1_0": "Tato so mudukā bhisiyā,",
  "ja545:25.2_0": "muducittakasanthate;",
  "ja545:25.3_0": "Mudupaccatthate rājā,",
  "ja545:25.4_0": "ekamantaṁ upāvisi.",
  "ja545:26.1_0": "Nisajja rājā sammodi,",
  "ja545:26.2_0": "kathaṁ sāraṇiyaṁ tato;",
  "ja545:26.3_0": "“Kacci yāpaniyaṁ bhante,",
  "ja545:26.4_0": "vātānamaviyaggatā.",
  "ja545:27.1_0": "Kacci akasirā vutti,",
  "ja545:27.2_0": "labhasi piṇḍayāpanaṁ;",
  "ja545:27.3_0": "Apābādho casi kacci,",
  "ja545:27.4_0": "cakkhuṁ na parihāyati”.",
  "ja545:28.1_0": "Taṁ guṇo paṭisammodi,",
  "ja545:28.2_0": "vedehaṁ vinaye rataṁ;",
  "ja545:28.3_0": "“Yāpanīyaṁ mahārāja,",
  "ja545:28.4_0": "sabbametaṁ tadūbhayaṁ.",
  "ja545:29.1_0": "Kacci tuyhampi vedeha,",
  "ja545:29.2_0": "paccantā na balīyare;",
  "ja545:29.3_0": "Kacci arogaṁ yoggaṁ te,",
  "ja545:29.4_0": "kacci vahati vāhanaṁ;",
  "ja545:29.5_0": "Kacci te byādhayo natthi,",
  "ja545:29.6_0": "sarīrassupatāpiyā”.",
  "ja545:30.1_0": "Paṭisammodito rājā,",
  "ja545:30.2_0": "tato pucchi anantarā;",
  "ja545:30.3_0": "Atthaṁ dhammañca ñāyañca,",
  "ja545:30.4_0": "dhammakāmo rathesabho.",
  "ja545:31.1_0": "“Kathaṁ dhammaṁ care macco,",
  "ja545:31.2_0": "mātāpitūsu kassapa;",
  "ja545:31.3_0": "Kathaṁ care ācariye,",
  "ja545:31.4_0": "puttadāre kathaṁ care.",
  "ja545:32.1_0": "Kathaṁ careyya vuḍḍhesu,",
  "ja545:32.2_0": "kathaṁ samaṇabrāhmaṇe;",
  "ja545:32.3_0": "Kathañca balakāyasmiṁ,",
  "ja545:32.4_0": "kathaṁ janapade care.",
  "ja545:33.1_0": "Kathaṁ dhammaṁ caritvāna,",
  "ja545:33.2_0": "maccā gacchanti suggatiṁ;",
  "ja545:33.3_0": "Kathañceke adhammaṭṭhā,",
  "ja545:33.4_0": "patanti nirayaṁ atho”.",
  "ja545:34.1_0": "Vedehassa vaco sutvā,",
  "ja545:34.2_0": "kassapo etadabravi;",
  "ja545:34.3_0": "“Suṇohi me mahārāja,",
  "ja545:34.4_0": "saccaṁ avitathaṁ padaṁ.",
  "ja545:35.1_0": "Natthi dhammacaritassa,",
  "ja545:35.2_0": "phalaṁ kalyāṇapāpakaṁ;",
  "ja545:35.3_0": "Natthi deva paro loko,",
  "ja545:35.4_0": "ko tato hi idhāgato.",
  "ja545:36.1_0": "Natthi deva pitaro vā,",
  "ja545:36.2_0": "kuto mātā kuto pitā;",
  "ja545:36.3_0": "Natthi ācariyo nāma,",
  "ja545:36.4_0": "adantaṁ ko damessati.",
  "ja545:37.1_0": "Samatulyāni bhūtāni,",
  "ja545:37.2_0": "natthi jeṭṭhāpacāyikā;",
  "ja545:37.3_0": "Natthi balaṁ vīriyaṁ vā,",
  "ja545:37.4_0": "kuto uṭṭhānaporisaṁ;",
  "ja545:37.5_0": "Niyatāni hi bhūtāni,",
  "ja545:37.6_0": "yathā goṭaviso tathā.",
  "ja545:38.1_0": "Laddheyyaṁ labhate macco,",
  "ja545:38.2_0": "tattha dānaphalaṁ kuto;",
  "ja545:38.3_0": "Natthi dānaphalaṁ deva,",
  "ja545:38.4_0": "avaso devavīriyo.",
  "ja545:39.1_0": "Bālehi dānaṁ paññattaṁ,",
  "ja545:39.2_0": "paṇḍitehi paṭicchitaṁ;",
  "ja545:39.3_0": "Avasā denti dhīrānaṁ,",
  "ja545:39.4_0": "bālā paṇḍitamānino.",
  "ja545:40.1_0": "Sattime sassatā kāyā,",
  "ja545:40.2_0": "acchejjā avikopino;",
  "ja545:40.3_0": "Tejo pathavī āpo ca,",
  "ja545:40.4_0": "vāyo sukhaṁ dukhañcime;",
  "ja545:40.5_0": "Jīve ca sattime kāyā,",
  "ja545:40.6_0": "yesaṁ chettā na vijjati.",
  "ja545:41.1_0": "Natthi hantā va chettā vā,",
  "ja545:41.2_0": "haññe yevāpi koci naṁ;",
  "ja545:41.3_0": "Antareneva kāyānaṁ,",
  "ja545:41.4_0": "satthāni vītivattare.",
  "ja545:42.1_0": "Yo cāpi siramādāya,",
  "ja545:42.2_0": "paresaṁ nisitāsinā;",
  "ja545:42.3_0": "Na so chindati te kāye,",
  "ja545:42.4_0": "tattha pāpaphalaṁ kuto.",
  "ja545:43.1_0": "Cullāsītimahākappe,",
  "ja545:43.2_0": "sabbe sujjhanti saṁsaraṁ;",
  "ja545:43.3_0": "Anāgate tamhi kāle,",
  "ja545:43.4_0": "saññatopi na sujjhati.",
  "ja545:44.1_0": "Caritvāpi bahuṁ bhadraṁ,",
  "ja545:44.2_0": "neva sujjhantināgate;",
  "ja545:44.3_0": "Pāpañcepi bahuṁ katvā,",
  "ja545:44.4_0": "taṁ khaṇaṁ nātivattare.",
  "ja545:45.1_0": "Anupubbena no suddhi,",
  "ja545:45.2_0": "kappānaṁ cullasītiyā;",
  "ja545:45.3_0": "Niyatiṁ nātivattāma,",
  "ja545:45.4_0": "velantamiva sāgaro”.",
  "ja545:46.1_0": "Kassapassa vaco sutvā,",
  "ja545:46.2_0": "alāto etadabravi;",
  "ja545:46.3_0": "“Yathā bhadanto bhaṇati,",
  "ja545:46.4_0": "mayhampetaṁva ruccati.",
  "ja545:47.1_0": "Ahampi purimaṁ jātiṁ,",
  "ja545:47.2_0": "sare saṁsaritattano;",
  "ja545:47.3_0": "Piṅgalo nāmahaṁ āsiṁ,",
  "ja545:47.4_0": "luddo goghātako pure.",
  "ja545:48.1_0": "Bārāṇasiyaṁ phītāyaṁ,",
  "ja545:48.2_0": "bahuṁ pāpaṁ kataṁ mayā;",
  "ja545:48.3_0": "Bahū mayā hatā pāṇā,",
  "ja545:48.4_0": "mahiṁsā sūkarā ajā.",
  "ja545:49.1_0": "Tato cuto idha jāto,",
  "ja545:49.2_0": "iddhe senāpatīkule;",
  "ja545:49.3_0": "Natthi nūna phalaṁ pāpaṁ,",
  "ja545:49.4_0": "yohaṁ na nirayaṁ gato”.",
  "ja545:50.1_0": "Athettha bījako nāma,",
  "ja545:50.2_0": "dāso āsi paṭaccarī;",
  "ja545:50.3_0": "Uposathaṁ upavasanto,",
  "ja545:50.4_0": "guṇasantikupāgami.",
  "ja545:51.1_0": "Kassapassa vaco sutvā,",
  "ja545:51.2_0": "alātassa ca bhāsitaṁ;",
  "ja545:51.3_0": "Passasanto muhuṁ uṇhaṁ,",
  "ja545:51.4_0": "rudaṁ assūni vattayi.",
  "ja545:52.1_0": "Tamanupucchi vedeho,",
  "ja545:52.2_0": "“kimatthaṁ samma rodasi;",
  "ja545:52.3_0": "Kiṁ te sutaṁ vā diṭṭhaṁ vā,",
  "ja545:52.4_0": "kiṁ maṁ vedesi vedanaṁ”.",
  "ja545:53.1_0": "Vedehassa vaco sutvā,",
  "ja545:53.2_0": "bījako etadabravi;",
  "ja545:53.3_0": "“Natthi me vedanā dukkhā,",
  "ja545:53.4_0": "mahārāja suṇohi me.",
  "ja545:54.1_0": "Ahampi purimaṁ jātiṁ,",
  "ja545:54.2_0": "sarāmi sukhamattano;",
  "ja545:54.3_0": "Sāketāhaṁ pure āsiṁ,",
  "ja545:54.4_0": "bhāvaseṭṭhi guṇe rato.",
  "ja545:55.1_0": "Sammato brāhmaṇibbhānaṁ,",
  "ja545:55.2_0": "saṁvibhāgarato suci;",
  "ja545:55.3_0": "Na cāpi pāpakaṁ kammaṁ,",
  "ja545:55.4_0": "sarāmi katamattano.",
  "ja545:56.1_0": "Tato cutāhaṁ vedeha,",
  "ja545:56.2_0": "idha jāto duritthiyā;",
  "ja545:56.3_0": "Gabbhamhi kumbhadāsiyā,",
  "ja545:56.4_0": "yato jāto suduggato.",
  "ja545:57.1_0": "Evampi duggato santo,",
  "ja545:57.2_0": "samacariyaṁ adhiṭṭhito;",
  "ja545:57.3_0": "Upaḍḍhabhāgaṁ bhattassa,",
  "ja545:57.4_0": "dadāmi yo me icchati.",
  "ja545:58.1_0": "Cātuddasiṁ pañcadasiṁ,",
  "ja545:58.2_0": "sadā upavasāmahaṁ;",
  "ja545:58.3_0": "Na cāpi bhūte hiṁsāmi,",
  "ja545:58.4_0": "theyyañcāpi vivajjayiṁ.",
  "ja545:59.1_0": "Sabbameva hi nūnetaṁ,",
  "ja545:59.2_0": "suciṇṇaṁ bhavati nipphalaṁ;",
  "ja545:59.3_0": "Niratthaṁ maññidaṁ sīlaṁ,",
  "ja545:59.4_0": "alāto bhāsatī yathā.",
  "ja545:60.1_0": "Kalimeva nūna gaṇhāmi,",
  "ja545:60.2_0": "asippo dhuttako yathā;",
  "ja545:60.3_0": "Kaṭaṁ alāto gaṇhāti,",
  "ja545:60.4_0": "kitavosikkhito yathā.",
  "ja545:61.1_0": "Dvāraṁ nappaṭipassāmi,",
  "ja545:61.2_0": "yena gacchāmi suggatiṁ;",
  "ja545:61.3_0": "Tasmā rāja parodāmi,",
  "ja545:61.4_0": "sutvā kassapabhāsitaṁ”.",
  "ja545:62.1_0": "Bījakassa vaco sutvā,",
  "ja545:62.2_0": "rājā aṅgati mabravi;",
  "ja545:62.3_0": "“Natthi dvāraṁ sugatiyā,",
  "ja545:62.4_0": "niyatiṁ kaṅkha bījaka.",
  "ja545:63.1_0": "Sukhaṁ vā yadi vā dukkhaṁ,",
  "ja545:63.2_0": "niyatiyā kira labbhati;",
  "ja545:63.3_0": "Saṁsārasuddhi sabbesaṁ,",
  "ja545:63.4_0": "mā turittho anāgate.",
  "ja545:64.1_0": "Ahampi pubbe kalyāṇo,",
  "ja545:64.2_0": "brāhmaṇibbhesu byāvaṭo;",
  "ja545:64.3_0": "Vohāramanusāsanto,",
  "ja545:64.4_0": "ratihīno tadantarā.",
  "ja545:65.1_0": "Punapi bhante dakkhemu,",
  "ja545:65.2_0": "saṅgati ce bhavissati”;",
  "ja545:65.3_0": "Idaṁ vatvāna vedeho,",
  "ja545:65.4_0": "paccagā sanivesanaṁ.",
  "ja545:66.1_0": "Tato ratyā vivasāne,",
  "ja545:66.2_0": "upaṭṭhānamhi aṅgati;",
  "ja545:66.3_0": "Amacce sannipātetvā,",
  "ja545:66.4_0": "idaṁ vacanamabravi.",
  "ja545:67.1_0": "“Candake me vimānasmiṁ,",
  "ja545:67.2_0": "sadā kāme vidhentu me;",
  "ja545:67.3_0": "Mā upagacchuṁ atthesu,",
  "ja545:67.4_0": "guyhappakāsiyesu ca.",
  "ja545:68.1_0": "Vijayo ca sunāmo ca,",
  "ja545:68.2_0": "senāpati alātako;",
  "ja545:68.3_0": "Ete atthe nisīdantu,",
  "ja545:68.4_0": "vohārakusalā tayo”.",
  "ja545:69.1_0": "Idaṁ vatvāna vedeho,",
  "ja545:69.2_0": "“kāmeva bahumaññatha;",
  "ja545:69.3_0": "Na cāpi brāhmaṇibbhesu,",
  "ja545:69.4_0": "atthe kismiñci byāvaṭo”.",
  "ja545:70.1_0": "Tato dvesattarattassa,",
  "ja545:70.2_0": "vedehassatrajā piyā;",
  "ja545:70.3_0": "Rājakaññā rucā nāma,",
  "ja545:70.4_0": "dhātimātaramabravi.",
  "ja545:71.1_0": "“Alaṅkarotha maṁ khippaṁ,",
  "ja545:71.2_0": "sakhiyo cālaṅkarontu me;",
  "ja545:71.3_0": "Suve pannaraso dibyo,",
  "ja545:71.4_0": "gacchaṁ issarasantike”.",
  "ja545:72.1_0": "Tassā mālyaṁ abhihariṁsu,",
  "ja545:72.2_0": "candanañca mahārahaṁ;",
  "ja545:72.3_0": "Maṇisaṅkhamuttāratanaṁ,",
  "ja545:72.4_0": "nānāratte ca ambare.",
  "ja545:73.1_0": "Tañca soṇṇamaye pīṭhe,",
  "ja545:73.2_0": "nisinnaṁ bahukitthiyo;",
  "ja545:73.3_0": "Parikiriya pasobhiṁsu,",
  "ja545:73.4_0": "rucaṁ ruciravaṇṇiniṁ.",
  "ja545:74.1_0": "Sā ca sakhimajjhagatā,",
  "ja545:74.2_0": "sabbābharaṇabhūsitā;",
  "ja545:74.3_0": "Sateratā abbhamiva,",
  "ja545:74.4_0": "candakaṁ pāvisī rucā.",
  "ja545:75.1_0": "Upasaṅkamitvā vedehaṁ,",
  "ja545:75.2_0": "vanditvā vinaye rataṁ;",
  "ja545:75.3_0": "Suvaṇṇakhacite pīṭhe,",
  "ja545:75.4_0": "ekamantaṁ upāvisi.",
  "ja545:76.1_0": "Tañca disvāna vedeho,",
  "ja545:76.2_0": "accharānaṁva saṅgamaṁ;",
  "ja545:76.3_0": "Rucaṁ sakhimajjhagataṁ,",
  "ja545:76.4_0": "idaṁ vacanamabravi.",
  "ja545:77.1_0": "“Kacci ramasi pāsāde,",
  "ja545:77.2_0": "antopokkharaṇiṁ pati;",
  "ja545:77.3_0": "Kacci bahuvidhaṁ khajjaṁ,",
  "ja545:77.4_0": "sadā abhiharanti te.",
  "ja545:78.1_0": "Kacci bahuvidhaṁ mālyaṁ,",
  "ja545:78.2_0": "ocinitvā kumāriyo;",
  "ja545:78.3_0": "Gharake karotha paccekaṁ,",
  "ja545:78.4_0": "khiḍḍāratiratā muhuṁ.",
  "ja545:79.1_0": "Kena vā vikalaṁ tuyhaṁ,",
  "ja545:79.2_0": "kiṁ khippaṁ āharantu te;",
  "ja545:79.3_0": "Manokarassu kuḍḍamukhī,",
  "ja545:79.4_0": "api candasamamhipi”.",
  "ja545:80.1_0": "Vedehassa vaco sutvā,",
  "ja545:80.2_0": "rucā pitaramabravi;",
  "ja545:80.3_0": "“Sabbametaṁ mahārāja,",
  "ja545:80.4_0": "labbhatissarasantike.",
  "ja545:81.1_0": "Suve pannaraso dibyo,",
  "ja545:81.2_0": "sahassaṁ āharantu me;",
  "ja545:81.3_0": "Yathādinnañca dassāmi,",
  "ja545:81.4_0": "dānaṁ sabbavanīsvahaṁ”.",
  "ja545:82.1_0": "Rucāya vacanaṁ sutvā,",
  "ja545:82.2_0": "rājā aṅgati mabravi;",
  "ja545:82.3_0": "“Bahuṁ vināsitaṁ vittaṁ,",
  "ja545:82.4_0": "niratthaṁ aphalaṁ tayā.",
  "ja545:83.1_0": "Uposathe vasaṁ niccaṁ,",
  "ja545:83.2_0": "annapānaṁ na bhuñjasi;",
  "ja545:83.3_0": "Niyatetaṁ abhuttabbaṁ,",
  "ja545:83.4_0": "natthi puññaṁ abhuñjato.",
  "ja545:84.1_0": "Bījakopi hi sutvāna,",
  "ja545:84.2_0": "tadā kassapabhāsitaṁ;",
  "ja545:84.3_0": "Passasanto muhuṁ uṇhaṁ,",
  "ja545:84.4_0": "rudaṁ assūni vattayi.",
  "ja545:85.1_0": "Yāva ruce jīvamānā,",
  "ja545:85.2_0": "mā bhattamapanāmayi;",
  "ja545:85.3_0": "Natthi bhadde paro loko,",
  "ja545:85.4_0": "kiṁ niratthaṁ vihaññasi”.",
  "ja545:86.1_0": "Vedehassa vaco sutvā,",
  "ja545:86.2_0": "rucā ruciravaṇṇinī;",
  "ja545:86.3_0": "Jānaṁ pubbāparaṁ dhammaṁ,",
  "ja545:86.4_0": "pitaraṁ etadabravi.",
  "ja545:87.1_0": "“Sutameva pure āsi,",
  "ja545:87.2_0": "sakkhi diṭṭhamidaṁ mayā;",
  "ja545:87.3_0": "Bālūpasevī yo hoti,",
  "ja545:87.4_0": "bālova samapajjatha.",
  "ja545:88.1_0": "Mūḷho hi mūḷhamāgamma,",
  "ja545:88.2_0": "bhiyyo mohaṁ nigacchati;",
  "ja545:88.3_0": "Patirūpaṁ alātena,",
  "ja545:88.4_0": "bījakena ca muyhituṁ.",
  "ja545:89.1_0": "Tvañca devāsi sappañño,",
  "ja545:89.2_0": "dhiro atthassa kovido;",
  "ja545:89.3_0": "Kathaṁ bālehi sadisaṁ,",
  "ja545:89.4_0": "hīnadiṭṭhiṁ upāgami.",
  "ja545:90.1_0": "Sacepi saṁsārapathena sujjhati,",
  "ja545:90.2_0": "Niratthiyā pabbajjā guṇassa;",
  "ja545:90.3_0": "Kīṭova aggiṁ jalitaṁ apāpataṁ,",
  "ja545:90.4_0": "Upapajjati mohamūḷho naggabhāvaṁ.",
  "ja545:91.1_0": "Saṁsārasuddhīti pure niviṭṭhā,",
  "ja545:91.2_0": "Kammaṁ vidūsenti bahū ajānaṁ;",
  "ja545:91.3_0": "Pubbe kalī duggahitovanatthā,",
  "ja545:91.4_0": "Dummo ca yā balisā ambujova.",
  "ja545:92.1_0": "Upamaṁ te karissāmi,",
  "ja545:92.2_0": "mahārāja tavatthiyā;",
  "ja545:92.3_0": "Upamāya midhekacce,",
  "ja545:92.4_0": "atthaṁ jānanti paṇḍitā.",
  "ja545:93.1_0": "Vāṇijānaṁ yathā nāvā,",
  "ja545:93.2_0": "appamāṇabharā garu;",
  "ja545:93.3_0": "Atibhāraṁ samādāya,",
  "ja545:93.4_0": "aṇṇave avasīdati.",
  "ja545:94.1_0": "Evameva naro pāpaṁ,",
  "ja545:94.2_0": "thokaṁ thokampi ācinaṁ;",
  "ja545:94.3_0": "Atibhāraṁ samādāya,",
  "ja545:94.4_0": "niraye avasīdati.",
  "ja545:95.1_0": "Na tāva bhāro paripūro,",
  "ja545:95.2_0": "alātassa mahīpati;",
  "ja545:95.3_0": "Ācināti ca taṁ pāpaṁ,",
  "ja545:95.4_0": "yena gacchati duggatiṁ.",
  "ja545:96.1_0": "Pubbevassa kataṁ puññaṁ,",
  "ja545:96.2_0": "alātassa mahīpati;",
  "ja545:96.3_0": "Tasseva deva nissando,",
  "ja545:96.4_0": "yañceso labhate sukhaṁ.",
  "ja545:97.1_0": "Khīyate cassa taṁ puññaṁ,",
  "ja545:97.2_0": "tathā hi aguṇe rato;",
  "ja545:97.3_0": "Ujumaggaṁ avahāya,",
  "ja545:97.4_0": "kummaggamanudhāvati.",
  "ja545:98.1_0": "Tulā yathā paggahitā,",
  "ja545:98.2_0": "ohite tulamaṇḍale;",
  "ja545:98.3_0": "Unnameti tulāsīsaṁ,",
  "ja545:98.4_0": "bhāre oropite sati.",
  "ja545:99.1_0": "Evameva naro puññaṁ,",
  "ja545:99.2_0": "thokaṁ thokampi ācinaṁ;",
  "ja545:99.3_0": "Saggātimāno dāsova,",
  "ja545:99.4_0": "bījako sātave rato.",
  "ja545:100.1_0": "Yamajja bījako dāso,",
  "ja545:100.2_0": "dukkhaṁ passati attani;",
  "ja545:100.3_0": "Pubbevassa kataṁ pāpaṁ,",
  "ja545:100.4_0": "tameso paṭisevati.",
  "ja545:101.1_0": "Khīyate cassa taṁ pāpaṁ,",
  "ja545:101.2_0": "tathā hi vinaye rato;",
  "ja545:101.3_0": "Kassapañca samāpajja,",
  "ja545:101.4_0": "mā hevuppathamāgamā.",
  "ja545:102.1_0": "Yaṁ yañhi rāja bhajati,",
  "ja545:102.2_0": "santaṁ vā yadi vā asaṁ;",
  "ja545:102.3_0": "Sīlavantaṁ visīlaṁ vā,",
  "ja545:102.4_0": "vasaṁ tasseva gacchati.",
  "ja545:103.1_0": "Yādisaṁ kurute mittaṁ,",
  "ja545:103.2_0": "yādisaṁ cūpasevati;",
  "ja545:103.3_0": "Sopi tādisako hoti,",
  "ja545:103.4_0": "sahavāso hi tādiso.",
  "ja545:104.1_0": "Sevamāno sevamānaṁ,",
  "ja545:104.2_0": "samphuṭṭho samphusaṁ paraṁ;",
  "ja545:104.3_0": "Saro diddho kalāpaṁva,",
  "ja545:104.4_0": "alittamupalimpati;",
  "ja545:104.5_0": "Upalepabhayā dhīro,",
  "ja545:104.6_0": "neva pāpasakhā siyā.",
  "ja545:105.1_0": "Pūtimacchaṁ kusaggena,",
  "ja545:105.2_0": "yo naro upanayhati;",
  "ja545:105.3_0": "Kusāpi pūti vāyanti,",
  "ja545:105.4_0": "evaṁ bālūpasevanā.",
  "ja545:106.1_0": "Tagarañca palāsena,",
  "ja545:106.2_0": "yo naro upanayhati;",
  "ja545:106.3_0": "Pattāpi surabhi vāyanti,",
  "ja545:106.4_0": "evaṁ dhīrūpasevanā.",
  "ja545:107.1_0": "Tasmā pattapuṭasseva,",
  "ja545:107.2_0": "ñatvā sampākamattano;",
  "ja545:107.3_0": "Asante nopaseveyya,",
  "ja545:107.4_0": "sante seveyya paṇḍito;",
  "ja545:107.5_0": "Asanto nirayaṁ nenti,",
  "ja545:107.6_0": "santo pāpenti suggatiṁ.",
  "ja545:108.1_0": "Ahampi jātiyo satta,",
  "ja545:108.2_0": "sare saṁsaritattano;",
  "ja545:108.3_0": "Anāgatāpi satteva,",
  "ja545:108.4_0": "yā gamissaṁ ito cutā.",
  "ja545:109.1_0": "Yā me sā sattamī jāti,",
  "ja545:109.2_0": "ahu pubbe janādhipa;",
  "ja545:109.3_0": "Kammāraputto magadhesu,",
  "ja545:109.4_0": "ahuṁ rājagahe pure.",
  "ja545:110.1_0": "Pāpaṁ sahāyamāgamma,",
  "ja545:110.2_0": "bahuṁ pāpaṁ kataṁ mayā;",
  "ja545:110.3_0": "Paradārassa heṭhento,",
  "ja545:110.4_0": "carimhā amarā viya.",
  "ja545:111.1_0": "Taṁ kammaṁ nihitaṁ aṭṭhā,",
  "ja545:111.2_0": "bhasmacchannova pāvako;",
  "ja545:111.3_0": "Atha aññehi kammehi,",
  "ja545:111.4_0": "ajāyiṁ vaṁsabhūmiyaṁ.",
  "ja545:112.1_0": "Kosambiyaṁ seṭṭhikule,",
  "ja545:112.2_0": "iddhe phīte mahaddhane;",
  "ja545:112.3_0": "Ekaputto mahārāja,",
  "ja545:112.4_0": "niccaṁ sakkatapūjito.",
  "ja545:113.1_0": "Tattha mittaṁ asevissaṁ,",
  "ja545:113.2_0": "sahāyaṁ sātave rataṁ;",
  "ja545:113.3_0": "Paṇḍitaṁ sutasampannaṁ,",
  "ja545:113.4_0": "so maṁ atthe nivesayi.",
  "ja545:114.1_0": "Cātuddasiṁ pañcadasiṁ,",
  "ja545:114.2_0": "bahuṁ rattiṁ upāvasiṁ;",
  "ja545:114.3_0": "Taṁ kammaṁ nihitaṁ aṭṭhā,",
  "ja545:114.4_0": "nidhīva udakantike.",
  "ja545:115.1_0": "Atha pāpāna kammānaṁ,",
  "ja545:115.2_0": "Yametaṁ magadhe kataṁ;",
  "ja545:115.3_0": "Phalaṁ pariyāga maṁ pacchā,",
  "ja545:115.4_0": "Bhutvā duṭṭhavisaṁ yathā.",
  "ja545:116.1_0": "Tato cutāhaṁ vedeha,",
  "ja545:116.2_0": "roruve niraye ciraṁ;",
  "ja545:116.3_0": "Sakammunā apaccissaṁ,",
  "ja545:116.4_0": "taṁ saraṁ na sukhaṁ labhe.",
  "ja545:117.1_0": "Bahuvassagaṇe tattha,",
  "ja545:117.2_0": "khepayitvā bahuṁ dukhaṁ;",
  "ja545:117.3_0": "Bhinnāgate ahuṁ rāja,",
  "ja545:117.4_0": "chagalo uddhatapphalo.",
  "ja545:118.1_0": "Sātaputtā mayā vūḷhā,",
  "ja545:118.2_0": "piṭṭhiyā ca rathena ca;",
  "ja545:118.3_0": "Tassa kammassa nissando,",
  "ja545:118.4_0": "paradāragamanassa me.",
  "ja545:119.1_0": "Tato cutāhaṁ vedeha,",
  "ja545:119.2_0": "kapi āsiṁ brahāvane;",
  "ja545:119.3_0": "Niluñcitaphaloyeva,",
  "ja545:119.4_0": "yūthapena pagabbhinā;",
  "ja545:119.5_0": "Tassa kammassa nissando,",
  "ja545:119.6_0": "paradāragamanassa me.",
  "ja545:120.1_0": "Tato cutāhaṁ vedeha,",
  "ja545:120.2_0": "dassanesu pasū ahuṁ;",
  "ja545:120.3_0": "Niluñcito javo bhadro,",
  "ja545:120.4_0": "yoggaṁ vūḷhaṁ ciraṁ mayā;",
  "ja545:120.5_0": "Tassa kammassa nissando,",
  "ja545:120.6_0": "paradāragamanassa me.",
  "ja545:121.1_0": "Tato cutāhaṁ vedeha,",
  "ja545:121.2_0": "vajjīsu kulamāgamā;",
  "ja545:121.3_0": "Nevitthī na pumā āsiṁ,",
  "ja545:121.4_0": "manussatte sudullabhe;",
  "ja545:121.5_0": "Tassa kammassa nissando,",
  "ja545:121.6_0": "paradāragamanassa me.",
  "ja545:122.1_0": "Tato cutāhaṁ vedeha,",
  "ja545:122.2_0": "ajāyiṁ nandane vane;",
  "ja545:122.3_0": "Bhavane tāvatiṁsāhaṁ,",
  "ja545:122.4_0": "accharā kāmavaṇṇinī.",
  "ja545:123.1_0": "Vicittavatthābharaṇā,",
  "ja545:123.2_0": "āmuttamaṇikuṇḍalā;",
  "ja545:123.3_0": "Kusalā naccagītassa,",
  "ja545:123.4_0": "sakkassa paricārikā.",
  "ja545:124.1_0": "Tattha ṭhitāhaṁ vedeha,",
  "ja545:124.2_0": "sarāmi jātiyo imā;",
  "ja545:124.3_0": "Anāgatāpi satteva,",
  "ja545:124.4_0": "yā gamissaṁ ito cutā.",
  "ja545:125.1_0": "Pariyāgataṁ taṁ kusalaṁ,",
  "ja545:125.2_0": "yaṁ me kosambiyaṁ kataṁ;",
  "ja545:125.3_0": "Deve ceva manusse ca,",
  "ja545:125.4_0": "sandhāvissaṁ ito cutā.",
  "ja545:126.1_0": "Satta jacco mahārāja,",
  "ja545:126.2_0": "niccaṁ sakkatapūjitā;",
  "ja545:126.3_0": "Thībhāvāpi na muccissaṁ,",
  "ja545:126.4_0": "chaṭṭhā nigatiyo imā.",
  "ja545:127.1_0": "Sattamī ca gati deva,",
  "ja545:127.2_0": "devaputto mahiddhiko;",
  "ja545:127.3_0": "Pumā devo bhavissāmi,",
  "ja545:127.4_0": "devakāyasmimuttamo.",
  "ja545:128.1_0": "Ajjāpi santānamayaṁ,",
  "ja545:128.2_0": "mālaṁ ganthenti nandane;",
  "ja545:128.3_0": "Devaputto javo nāma,",
  "ja545:128.4_0": "yo me mālaṁ paṭicchati.",
  "ja545:129.1_0": "Muhutto viya so dibyo,",
  "ja545:129.2_0": "idha vassāni soḷasa;",
  "ja545:129.3_0": "Rattindivo ca so dibyo,",
  "ja545:129.4_0": "mānusiṁ saradosataṁ.",
  "ja545:130.1_0": "Iti kammāni anventi,",
  "ja545:130.2_0": "asaṅkheyyāpi jātiyo;",
  "ja545:130.3_0": "Kalyāṇaṁ yadi vā pāpaṁ,",
  "ja545:130.4_0": "na hi kammaṁ vinassati.",
  "ja545:131.1_0": "Yo icche puriso hotuṁ,",
  "ja545:131.2_0": "jātiṁ jātiṁ punappunaṁ;",
  "ja545:131.3_0": "Paradāraṁ vivajjeyya,",
  "ja545:131.4_0": "dhotapādova kaddamaṁ.",
  "ja545:132.1_0": "Yā icche puriso hotuṁ,",
  "ja545:132.2_0": "jātiṁ jātiṁ punappunaṁ;",
  "ja545:132.3_0": "Sāmikaṁ apacāyeyya,",
  "ja545:132.4_0": "indaṁva paricārikā.",
  "ja545:133.1_0": "Yo icche dibyabhogañca,",
  "ja545:133.2_0": "dibbamāyuṁ yasaṁ sukhaṁ;",
  "ja545:133.3_0": "Pāpāni parivajjetvā,",
  "ja545:133.4_0": "tividhaṁ dhammamācare.",
  "ja545:134.1_0": "Kāyena vācā manasā,",
  "ja545:134.2_0": "appamatto vicakkhaṇo;",
  "ja545:134.3_0": "Attano hoti atthāya,",
  "ja545:134.4_0": "itthī vā yadi vā pumā.",
  "ja545:135.1_0": "Ye kecime mānujā jīvaloke,",
  "ja545:135.2_0": "Yasassino sabbasamantabhogā;",
  "ja545:135.3_0": "Asaṁsayaṁ tehi pure suciṇṇaṁ,",
  "ja545:135.4_0": "Kammassakāse puthu sabbasattā.",
  "ja545:136.1_0": "Iṅghānucintesi sayampi deva,",
  "ja545:136.2_0": "Kutonidānā te imā janinda;",
  "ja545:136.3_0": "Yā te imā accharāsannikāsā,",
  "ja545:136.4_0": "Alaṅkatā kañcanajālachannā”.",
  "ja545:137.1_0": "Iccevaṁ pitaraṁ kaññā,",
  "ja545:137.2_0": "rucā tosesi aṅgatiṁ;",
  "ja545:137.3_0": "Mūḷhassa maggamācikkhi,",
  "ja545:137.4_0": "dhammamakkhāsi subbatā.",
  "ja545:138.1_0": "Athāgamā brahmalokā,",
  "ja545:138.2_0": "nārado mānusiṁ pajaṁ;",
  "ja545:138.3_0": "Jambudīpaṁ avekkhanto,",
  "ja545:138.4_0": "addā rājānamaṅgatiṁ.",
  "ja545:139.1_0": "Tato patiṭṭhā pāsāde,",
  "ja545:139.2_0": "vedehassa puratthato;",
  "ja545:139.3_0": "Tañca disvānānuppattaṁ,",
  "ja545:139.4_0": "rucā isimavandatha.",
  "ja545:140.1_0": "Athāsanamhā oruyha,",
  "ja545:140.2_0": "rājā byathitamānaso;",
  "ja545:140.3_0": "Nāradaṁ paripucchanto,",
  "ja545:140.4_0": "idaṁ vacanamabravi.",
  "ja545:141.1_0": "“Kuto nu āgacchasi devavaṇṇi,",
  "ja545:141.2_0": "Obhāsayaṁ sabbadisā candimāva;",
  "ja545:141.3_0": "Akkhāhi me pucchito nāmagottaṁ,",
  "ja545:141.4_0": "Kathaṁ taṁ jānanti manussaloke”.",
  "ja545:142.1_0": "“Ahañhi devato idāni emi,",
  "ja545:142.2_0": "Obhāsayaṁ sabbadisā candimāva;",
  "ja545:142.3_0": "Akkhāmi te pucchito nāmagottaṁ,",
  "ja545:142.4_0": "Jānanti maṁ nārado kassapo ca”.",
  "ja545:143.1_0": "“Accherarūpaṁ tava yādisañca,",
  "ja545:143.2_0": "Vehāyasaṁ gacchasi tiṭṭhasī ca;",
  "ja545:143.3_0": "Pucchāmi taṁ nārada etamatthaṁ,",
  "ja545:143.4_0": "Atha kena vaṇṇena tavāyamiddhi”.",
  "ja545:144.1_0": "“Saccañca dhammo ca damo ca cāgo,",
  "ja545:144.2_0": "Guṇā mamete pakatā purāṇā;",
  "ja545:144.3_0": "Teheva dhammehi susevitehi,",
  "ja545:144.4_0": "Manojavo yena kāmaṁ gatosmi”.",
  "ja545:145.1_0": "“Accheramācikkhasi puññasiddhiṁ,",
  "ja545:145.2_0": "Sace hi etehi yathā vadesi;",
  "ja545:145.3_0": "Pucchāmi taṁ nārada etamatthaṁ,",
  "ja545:145.4_0": "Puṭṭho ca me sādhu viyākarohi”.",
  "ja545:146.1_0": "“Pucchassu maṁ rāja tavesa attho,",
  "ja545:146.2_0": "Yaṁ saṁsayaṁ kuruse bhūmipāla;",
  "ja545:146.3_0": "Ahaṁ taṁ nissaṁsayataṁ gamemi,",
  "ja545:146.4_0": "Nayehi ñāyehi ca hetubhī ca”.",
  "ja545:147.1_0": "“Pucchāmi taṁ nārada etamatthaṁ,",
  "ja545:147.2_0": "Puṭṭho ca me nārada mā musā bhaṇi;",
  "ja545:147.3_0": "Atthi nu devā pitaro nu atthi,",
  "ja545:147.4_0": "Loko paro atthi jano yamāhu”.",
  "ja545:148.1_0": "“Attheva devā pitaro ca atthi,",
  "ja545:148.2_0": "Loko paro atthi jano yamāhu;",
  "ja545:148.3_0": "Kāmesu giddhā ca narā pamūḷhā,",
  "ja545:148.4_0": "Lokaṁ paraṁ na vidū mohayuttā”.",
  "ja545:149.1_0": "“Atthīti ce nārada saddahāsi,",
  "ja545:149.2_0": "Nivesanaṁ paraloke matānaṁ;",
  "ja545:149.3_0": "Idheva me pañca satāni dehi,",
  "ja545:149.4_0": "Dassāmi te paraloke sahassaṁ”.",
  "ja545:150.1_0": "“Dajjemu kho pañca satāni bhoto,",
  "ja545:150.2_0": "Jaññāmu ce sīlavantaṁ vadaññuṁ;",
  "ja545:150.3_0": "Luddaṁ taṁ bhontaṁ niraye vasantaṁ,",
  "ja545:150.4_0": "Ko codaye paraloke sahassaṁ.",
  "ja545:151.1_0": "Idheva yo hoti adhammasīlo,",
  "ja545:151.2_0": "Pāpācāro alaso luddakammo;",
  "ja545:151.3_0": "Na paṇḍitā tasmiṁ iṇaṁ dadanti,",
  "ja545:151.4_0": "Na hi āgamo hoti tathāvidhamhā.",
  "ja545:152.1_0": "Dakkhañca posaṁ manujā viditvā,",
  "ja545:152.2_0": "Uṭṭhānakaṁ sīlavantaṁ vadaññuṁ;",
  "ja545:152.3_0": "Sayameva bhogehi nimantayanti,",
  "ja545:152.4_0": "Kammaṁ karitvā puna māharesi.",
  "ja545:153.1_0": "Ito cuto dakkhasi tattha rāja,",
  "ja545:153.2_0": "Kākolasaṅghehi vikassamānaṁ;",
  "ja545:153.3_0": "Taṁ khajjamānaṁ niraye vasantaṁ,",
  "ja545:153.4_0": "Kākehi gijjhehi ca senakehi;",
  "ja545:153.5_0": "Sañchinnagattaṁ ruhiraṁ savantaṁ,",
  "ja545:153.6_0": "Ko codaye paraloke sahassaṁ.",
  "ja545:154.1_0": "Andhaṁtamaṁ tattha na candasūriyā,",
  "ja545:154.2_0": "Nirayo sadā tumulo ghorarūpo;",
  "ja545:154.3_0": "Sā neva rattī na divā paññāyati,",
  "ja545:154.4_0": "Tathāvidhe ko vicare dhanatthiko.",
  "ja545:155.1_0": "Sabalo ca sāmo ca duve suvānā,",
  "ja545:155.2_0": "Pavaddhakāyā balino mahantā;",
  "ja545:155.3_0": "Khādanti dantehi ayomayehi,",
  "ja545:155.4_0": "Ito paṇunnaṁ paralokapattaṁ.",
  "ja545:156.1_0": "Taṁ khajjamānaṁ niraye vasantaṁ,",
  "ja545:156.2_0": "Luddehi vāḷehi aghammigehi ca;",
  "ja545:156.3_0": "Sañchinnagattaṁ ruhiraṁ savantaṁ,",
  "ja545:156.4_0": "Ko codaye paraloke sahassaṁ.",
  "ja545:157.1_0": "Usūhi sattīhi ca sunisitāhi,",
  "ja545:157.2_0": "Hananti vijjhanti ca paccamittā;",
  "ja545:157.3_0": "Kāḷūpakāḷā nirayamhi ghore,",
  "ja545:157.4_0": "Pubbe naraṁ dukkaṭakammakāriṁ.",
  "ja545:158.1_0": "Taṁ haññamānaṁ niraye vajantaṁ,",
  "ja545:158.2_0": "Kucchismiṁ passasmiṁ vipphālitūdaraṁ;",
  "ja545:158.3_0": "Sañchinnagattaṁ ruhiraṁ savantaṁ,",
  "ja545:158.4_0": "Ko codaye paraloke sahassaṁ.",
  "ja545:159.1_0": "Sattī usū tomarabhiṇḍivālā,",
  "ja545:159.2_0": "Vividhāvudhā vassanti tattha devā;",
  "ja545:159.3_0": "Patanti aṅgāramivaccimanto,",
  "ja545:159.4_0": "Silāsanī vassati luddakamme.",
  "ja545:160.1_0": "Uṇho ca vāto nirayamhi dussaho,",
  "ja545:160.2_0": "Na tamhi sukhaṁ labbhati ittarampi;",
  "ja545:160.3_0": "Taṁ taṁ vidhāvantamalenamāturaṁ,",
  "ja545:160.4_0": "Ko codaye paraloke sahassaṁ.",
  "ja545:161.1_0": "Sandhāvamānampi rathesu yuttaṁ,",
  "ja545:161.2_0": "Sajotibhūtaṁ pathaviṁ kamantaṁ;",
  "ja545:161.3_0": "Patodalaṭṭhīhi sucodayantaṁ,",
  "ja545:161.4_0": "Ko codaye paraloke sahassaṁ.",
  "ja545:162.1_0": "Tamāruhantaṁ khurasañcitaṁ giriṁ,",
  "ja545:162.2_0": "Vibhiṁsanaṁ pajjalitaṁ bhayānakaṁ;",
  "ja545:162.3_0": "Sañchinnagattaṁ ruhiraṁ savantaṁ,",
  "ja545:162.4_0": "Ko codaye paraloke sahassaṁ.",
  "ja545:163.1_0": "Tamāruhantaṁ pabbatasannikāsaṁ,",
  "ja545:163.2_0": "Aṅgārarāsiṁ jalitaṁ bhayānakaṁ;",
  "ja545:163.3_0": "Sudaḍḍhagattaṁ kapaṇaṁ rudantaṁ,",
  "ja545:163.4_0": "Ko codaye paraloke sahassaṁ.",
  "ja545:164.1_0": "Abbhakūṭasamā uccā,",
  "ja545:164.2_0": "kaṇṭakanicitā dumā;",
  "ja545:164.3_0": "Ayomayehi tikkhehi,",
  "ja545:164.4_0": "naralohitapāyibhi.",
  "ja545:165.1_0": "Tamāruhanti nāriyo,",
  "ja545:165.2_0": "narā ca paradāragū;",
  "ja545:165.3_0": "Coditā sattihatthehi,",
  "ja545:165.4_0": "yamaniddesakāribhi.",
  "ja545:166.1_0": "Tamāruhantaṁ nirayaṁ,",
  "ja545:166.2_0": "simbaliṁ ruhiramakkhitaṁ;",
  "ja545:166.3_0": "Vidaḍḍhakāyaṁ vitacaṁ,",
  "ja545:166.4_0": "āturaṁ gāḷhavedanaṁ.",
  "ja545:167.1_0": "Passasantaṁ muhuṁ uṇhaṁ,",
  "ja545:167.2_0": "pubbakammāparādhikaṁ;",
  "ja545:167.3_0": "Dumagge vitacaṁ gattaṁ,",
  "ja545:167.4_0": "ko taṁ yāceyya taṁ dhanaṁ.",
  "ja545:168.1_0": "Abbhakūṭasamā uccā,",
  "ja545:168.2_0": "asipattācitā dumā;",
  "ja545:168.3_0": "Ayomayehi tikkhehi,",
  "ja545:168.4_0": "naralohitapāyibhi.",
  "ja545:169.1_0": "Tamāruhantaṁ asipattapādapaṁ,",
  "ja545:169.2_0": "Asīhi tikkhehi ca chijjamānaṁ;",
  "ja545:169.3_0": "Sañchinnagattaṁ ruhiraṁ savantaṁ,",
  "ja545:169.4_0": "Ko codaye paraloke sahassaṁ.",
  "ja545:170.1_0": "Tato nikkhantamattaṁ taṁ,",
  "ja545:170.2_0": "asipattācitā dumā;",
  "ja545:170.3_0": "Sampatitaṁ vetaraṇiṁ,",
  "ja545:170.4_0": "ko taṁ yāceyya taṁ dhanaṁ.",
  "ja545:171.1_0": "Kharā kharodakā tattā,",
  "ja545:171.2_0": "duggā vetaraṇī nadī;",
  "ja545:171.3_0": "Ayopokkharasañchannā,",
  "ja545:171.4_0": "tikkhā pattehi sandati.",
  "ja545:172.1_0": "Tattha sañchinnagattaṁ taṁ,",
  "ja545:172.2_0": "vuyhantaṁ ruhiramakkhitaṁ;",
  "ja545:172.3_0": "Vetaraññe anālambe,",
  "ja545:172.4_0": "ko taṁ yāceyya taṁ dhanaṁ”.",
  "ja545:173.1_0": "“Vedhāmi rukkho viya chijjamāno,",
  "ja545:173.2_0": "Disaṁ na jānāmi pamūḷhasañño;",
  "ja545:173.3_0": "Bhayānutappāmi mahā ca me bhayā,",
  "ja545:173.4_0": "Sutvāna kathā tava bhāsitā ise.",
  "ja545:174.1_0": "Āditte vārimajjhaṁva,",
  "ja545:174.2_0": "dīpaṁvoghe mahaṇṇave;",
  "ja545:174.3_0": "Andhakāreva pajjoto,",
  "ja545:174.4_0": "tvaṁ nosi saraṇaṁ ise.",
  "ja545:175.1_0": "Atthañca dhammaṁ anusāsa maṁ ise,",
  "ja545:175.2_0": "Atītamaddhā aparādhitaṁ mayā;",
  "ja545:175.3_0": "Ācikkha me nārada suddhimaggaṁ,",
  "ja545:175.4_0": "Yathā ahaṁ no nirayaṁ pateyyaṁ”.",
  "ja545:176.1_0": "“Yathā ahū dhataraṭṭho,",
  "ja545:176.2_0": "Vessāmitto aṭṭhako yāmataggi;",
  "ja545:176.3_0": "Usindaro cāpi sivī ca rājā,",
  "ja545:176.4_0": "Paricārakā samaṇabrāhmaṇānaṁ.",
  "ja545:177.1_0": "Ete caññe ca rājāno,",
  "ja545:177.2_0": "ye saggavisayaṁ gatā;",
  "ja545:177.3_0": "Adhammaṁ parivajjetvā,",
  "ja545:177.4_0": "dhammaṁ cara mahīpati.",
  "ja545:178.1_0": "Annahatthā ca te byamhe,",
  "ja545:178.2_0": "ghosayantu pure tava;",
  "ja545:178.3_0": "Ko chāto ko ca tasito,",
  "ja545:178.4_0": "ko mālaṁ ko vilepanaṁ;",
  "ja545:178.5_0": "Nānārattānaṁ vatthānaṁ,",
  "ja545:178.6_0": "ko naggo paridahissati.",
  "ja545:179.1_0": "Ko panthe chattamāneti,",
  "ja545:179.2_0": "pādukā ca mudū subhā;",
  "ja545:179.3_0": "Iti sāyañca pāto ca,",
  "ja545:179.4_0": "ghosayantu pure tava.",
  "ja545:180.1_0": "Jiṇṇaṁ posaṁ gavāssañca,",
  "ja545:180.2_0": "māssu yuñja yathā pure;",
  "ja545:180.3_0": "Parihārañca dajjāsi,",
  "ja545:180.4_0": "adhikārakato balī.",
  "ja545:181.1_0": "Kāyo te rathasaññāto,",
  "ja545:181.2_0": "manosārathiko lahu;",
  "ja545:181.3_0": "Avihiṁsāsāritakkho,",
  "ja545:181.4_0": "saṁvibhāgapaṭicchado.",
  "ja545:182.1_0": "Pādasaññamanemiyo,",
  "ja545:182.2_0": "hatthasaññamapakkharo;",
  "ja545:182.3_0": "Kucchisaññamanabbhanto,",
  "ja545:182.4_0": "vācāsaññamakūjano.",
  "ja545:183.1_0": "Saccavākyasamattaṅgo,",
  "ja545:183.2_0": "apesuññasusaññato;",
  "ja545:183.3_0": "Girāsakhilanelaṅgo,",
  "ja545:183.4_0": "mitabhāṇisilesito.",
  "ja545:184.1_0": "Saddhālobhasusaṅkhāro,",
  "ja545:184.2_0": "nivātañjalikubbaro;",
  "ja545:184.3_0": "Athaddhatānatīsāko,",
  "ja545:184.4_0": "sīlasaṁvaranandhano.",
  "ja545:185.1_0": "Akkodhanamanugghātī,",
  "ja545:185.2_0": "dhammapaṇḍarachattako;",
  "ja545:185.3_0": "Bāhusaccamapālambo,",
  "ja545:185.4_0": "ṭhitacittamupādhiyo.",
  "ja545:186.1_0": "Kālaññutācittasāro,",
  "ja545:186.2_0": "vesārajjatidaṇḍako;",
  "ja545:186.3_0": "Nivātavuttiyottako,",
  "ja545:186.4_0": "anatimānayugo lahu.",
  "ja545:187.1_0": "Alīnacittasanthāro,",
  "ja545:187.2_0": "vuddhisevī rajohato;",
  "ja545:187.3_0": "Sati patodo dhīrassa,",
  "ja545:187.4_0": "dhiti yogo ca rasmiyo.",
  "ja545:188.1_0": "Mano dantaṁ pathaṁ neti,",
  "ja545:188.2_0": "samadantehi vāhibhi;",
  "ja545:188.3_0": "Icchā lobho ca kummaggo,",
  "ja545:188.4_0": "ujumaggo ca saṁyamo.",
  "ja545:189.1_0": "Rūpe sadde rase gandhe,",
  "ja545:189.2_0": "vāhanassa padhāvato;",
  "ja545:189.3_0": "Paññā ākoṭanī rāja,",
  "ja545:189.4_0": "tattha attāva sārathi.",
  "ja545:190.1_0": "Sace etena yānena,",
  "ja545:190.2_0": "samacariyā daḷhā dhiti;",
  "ja545:190.3_0": "Sabbakāmaduho rāja,",
  "ja545:190.4_0": "na jātu nirayaṁ vaje”.",
  "ja545:191.1_0": "“Alāto devadattosi,",
  "ja545:191.2_0": "sunāmo āsi bhaddaji;",
  "ja545:191.3_0": "Vijayo sāriputtosi,",
  "ja545:191.4_0": "moggallānosi bījako.",
  "ja545:192.1_0": "Sunakkhatto licchaviputto,",
  "ja545:192.2_0": "guṇo āsi acelako;",
  "ja545:192.3_0": "Ānando sā rucā āsi,",
  "ja545:192.4_0": "yā rājānaṁ pasādayi.",
  "ja545:193.1_0": "Uruvelakassapo rājā,",
  "ja545:193.2_0": "pāpadiṭṭhi tadā ahu;",
  "ja545:193.3_0": "Mahābrahmā bodhisatto,",
  "ja545:193.4_0": "evaṁ dhāretha jātakan”ti.",
  "ja545:194.1_0": "Mahānāradakassapajātakaṁ aṭṭhamaṁ."
}