{
  "ja150:0.1_0": "Jātaka",
  "ja150:0.2_0": "Ekakanipāta",
  "ja150:0.3_0": "Kakaṇṭakavagga",
  "ja150:0.4_0": "10. Sañjīvajātaka",
  "ja150:1.1_0": "“Asantaṁ yo paggaṇhāti,",
  "ja150:1.2_0": "asantaṁ cūpasevati;",
  "ja150:1.3_0": "Tameva ghāsaṁ kurute,",
  "ja150:1.4_0": "byaggho sañjīvako yathā”ti.",
  "ja150:2.1_0": "Sañjīvajātakaṁ dasamaṁ.",
  "ja150:2.2_0": "Kakaṇṭakavaggo pannarasamo.",
  "ja150:3.1_0": "Tassuddānaṁ",
  "ja150:4.1_0": "Sukhamedhati daṇḍavaro ca puna,",
  "ja150:4.2_0": "Lasi vāladhi pañcamarādhavaro;",
  "ja150:4.3_0": "Samahodadhi kattika bondi puna,",
  "ja150:4.4_0": "Caturaṅgulabyagghavarena dasāti.",
  "ja150:5.1_0": "Uparimapaṇṇāsako.",
  "ja150:6.1_0": "Atha vagguddānaṁ",
  "ja150:7.1_0": "Apaṇṇakaṁ sīlavaggakuruṅga,",
  "ja150:7.2_0": "Kulāvakaṁ atthakāmena pañcamaṁ;",
  "ja150:7.3_0": "Āsīso itthivaruṇaṁ apāyi,",
  "ja150:7.4_0": "Littavaggena te dasa;",
  "ja150:7.5_0": "Parosataṁ haṁci kusanāḷi,",
  "ja150:7.6_0": "Asampadaṁ kakaṇṭakavaggo.",
  "ja150:8.1_0": "Ekanipātamhilaṅkatanti.",
  "ja150:9.1_0": "Ekakanipātaṁ niṭṭhitaṁ."
}