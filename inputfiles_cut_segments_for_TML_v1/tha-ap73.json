{
  "tha-ap73:0.1_0": "Therāpadāna",
  "tha-ap73:0.2_0": "Nāgasamālavagga",
  "tha-ap73:0.3_0": "1. Nāgasamālattheraapadāna",
  "tha-ap73:1.1_0": "“Āpāṭaliṁ ahaṁ pupphaṁ,",
  "tha-ap73:1.2_0": "ujjhitaṁ sumahāpathe;",
  "tha-ap73:1.3_0": "Thūpamhi abhiropesiṁ,",
  "tha-ap73:1.4_0": "sikhino lokabandhuno.",
  "tha-ap73:2.1_0": "Ekattiṁse ito kappe,",
  "tha-ap73:2.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap73:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap73:2.4_0": "thūpapūjāyidaṁ phalaṁ.",
  "tha-ap73:3.1_0": "Ito pannarase kappe,",
  "tha-ap73:3.2_0": "bhūmiyo nāma khattiyo;",
  "tha-ap73:3.3_0": "Sattaratanasampanno,",
  "tha-ap73:3.4_0": "cakkavattī mahabbalo.",
  "tha-ap73:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap73:4.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap73:4.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap73:4.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap73:5.1_0": "Itthaṁ sudaṁ āyasmā nāgasamālo thero imā gāthāyo abhāsitthāti.",
  "tha-ap73:6.1_0": "Nāgasamālattherassāpadānaṁ paṭhamaṁ."
}