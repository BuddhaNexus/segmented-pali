{
  "bv23:0.1_0": "Buddhavaṁsa",
  "bv23:0.2_0": "23. Vessabhūbuddhavaṁsa",
  "bv23:1.1_0": "“Tattheva maṇḍakappamhi,",
  "bv23:1.2_0": "asamo appaṭipuggalo;",
  "bv23:1.3_0": "Vessabhū nāma nāmena,",
  "bv23:1.4_0": "loke uppajji nāyako.",
  "bv23:2.1_0": "Ādittaṁ vata rāgaggi,",
  "bv23:2.2_0": "taṇhānaṁ vijitaṁ tadā;",
  "bv23:2.3_0": "Nāgova bandhanaṁ chetvā,",
  "bv23:2.4_0": "patto sambodhimuttamaṁ.",
  "bv23:3.1_0": "Dhammacakkaṁ pavattente,",
  "bv23:3.2_0": "vessabhūlokanāyake;",
  "bv23:3.3_0": "Asītikoṭisahassānaṁ,",
  "bv23:3.4_0": "paṭhamābhisamayo ahu.",
  "bv23:4.1_0": "Pakkante cārikaṁ raṭṭhe,",
  "bv23:4.2_0": "lokajeṭṭhe narāsabhe;",
  "bv23:4.3_0": "Sattatikoṭisahassānaṁ,",
  "bv23:4.4_0": "dutiyābhisamayo ahu.",
  "bv23:5.1_0": "Mahādiṭṭhiṁ vinodento,",
  "bv23:5.2_0": "pāṭiheraṁ karoti so;",
  "bv23:5.3_0": "Samāgatā naramarū,",
  "bv23:5.4_0": "dasasahassī sadevake.",
  "bv23:6.1_0": "Mahāacchariyaṁ disvā,",
  "bv23:6.2_0": "abbhutaṁ lomahaṁsanaṁ;",
  "bv23:6.3_0": "Devā ceva manussā ca,",
  "bv23:6.4_0": "bujjhare saṭṭhikoṭiyo.",
  "bv23:7.1_0": "Sannipātā tayo āsuṁ,",
  "bv23:7.2_0": "vessabhussa mahesino;",
  "bv23:7.3_0": "Khīṇāsavānaṁ vimalānaṁ,",
  "bv23:7.4_0": "santacittāna tādinaṁ.",
  "bv23:8.1_0": "Asītibhikkhusahassānaṁ,",
  "bv23:8.2_0": "paṭhamo āsi samāgamo;",
  "bv23:8.3_0": "Sattatibhikkhusahassānaṁ,",
  "bv23:8.4_0": "dutiyo āsi samāgamo.",
  "bv23:9.1_0": "Saṭṭhibhikkhusahassānaṁ,",
  "bv23:9.2_0": "tatiyo āsi samāgamo;",
  "bv23:9.3_0": "Jarādibhayabhītānaṁ,",
  "bv23:9.4_0": "orasānaṁ mahesino.",
  "bv23:10.1_0": "Ahaṁ tena samayena,",
  "bv23:10.2_0": "sudassano nāma khattiyo;",
  "bv23:10.3_0": "Nimantetvā mahāvīraṁ,",
  "bv23:10.4_0": "dānaṁ datvā mahārahaṁ;",
  "bv23:10.5_0": "Annapānena vatthena,",
  "bv23:10.6_0": "sasaṅghaṁ jinapūjayiṁ.",
  "bv23:11.1_0": "Tassa buddhassa asamassa,",
  "bv23:11.2_0": "cakkaṁ vattitamuttamaṁ;",
  "bv23:11.3_0": "Sutvāna paṇitaṁ dhammaṁ,",
  "bv23:11.4_0": "pabbajjamabhirocayiṁ.",
  "bv23:12.1_0": "Mahādānaṁ pavattetvā,",
  "bv23:12.2_0": "rattindivamatandito;",
  "bv23:12.3_0": "Pabbajjaṁ guṇasampannaṁ,",
  "bv23:12.4_0": "pabbajiṁ jinasantike.",
  "bv23:13.1_0": "Ācāraguṇasampanno,",
  "bv23:13.2_0": "vattasīlasamāhito;",
  "bv23:13.3_0": "Sabbaññutaṁ gavesanto,",
  "bv23:13.4_0": "ramāmi jinasāsane.",
  "bv23:14.1_0": "Saddhāpītiṁ upagantvā,",
  "bv23:14.2_0": "buddhaṁ vandāmi sattharaṁ;",
  "bv23:14.3_0": "Pīti uppajjati mayhaṁ,",
  "bv23:14.4_0": "bodhiyāyeva kāraṇā.",
  "bv23:15.1_0": "Anivattamānasaṁ ñatvā,",
  "bv23:15.2_0": "sambuddho etadabravi;",
  "bv23:15.3_0": "‘Ekatiṁse ito kappe,",
  "bv23:15.4_0": "ayaṁ buddho bhavissati.",
  "bv23:16.1_0": "Ahu kapilavhayā rammā,",
  "bv23:16.2_0": "…pe…",
  "bv23:16.3_0": "hessāma sammukhā imaṁ’.",
  "bv23:17.1_0": "Tassāhaṁ vacanaṁ sutvā,",
  "bv23:17.2_0": "bhiyyo cittaṁ pasādayiṁ;",
  "bv23:17.3_0": "Uttariṁ vatamadhiṭṭhāsiṁ,",
  "bv23:17.4_0": "dasapāramipūriyā.",
  "bv23:18.1_0": "Anomaṁ nāma nagaraṁ,",
  "bv23:18.2_0": "suppatīto nāma khattiyo;",
  "bv23:18.3_0": "Mātā yasavatī nāma,",
  "bv23:18.4_0": "vessabhussa mahesino.",
  "bv23:19.1_0": "Cha ca vassasahassāni,",
  "bv23:19.2_0": "Agāraṁ ajjha so vasi;",
  "bv23:19.3_0": "Ruci suruci rativaḍḍhano,",
  "bv23:19.4_0": "Tayo pāsādamuttamā.",
  "bv23:20.1_0": "Anūnatiṁsasahassāni,",
  "bv23:20.2_0": "nāriyo samalaṅkatā;",
  "bv23:20.3_0": "Sucittā nāma sā nārī,",
  "bv23:20.4_0": "suppabuddho nāma atrajo.",
  "bv23:21.1_0": "Nimitte caturo disvā,",
  "bv23:21.2_0": "sivikāyābhinikkhami;",
  "bv23:21.3_0": "Chamāsaṁ padhānacāraṁ,",
  "bv23:21.4_0": "acarī purisuttamo.",
  "bv23:22.1_0": "Brahmunā yācito santo,",
  "bv23:22.2_0": "vessabhūlokanāyako;",
  "bv23:22.3_0": "Vatti cakkaṁ mahāvīro,",
  "bv23:22.4_0": "aruṇārāme naruttamo.",
  "bv23:23.1_0": "Soṇo ca uttaro ceva,",
  "bv23:23.2_0": "ahesuṁ aggasāvakā;",
  "bv23:23.3_0": "Upasanto nāmupaṭṭhāko,",
  "bv23:23.4_0": "vessabhussa mahesino.",
  "bv23:24.1_0": "Rāmā ceva samālā ca,",
  "bv23:24.2_0": "ahesuṁ aggasāvikā;",
  "bv23:24.3_0": "Bodhi tassa bhagavato,",
  "bv23:24.4_0": "mahāsāloti vuccati.",
  "bv23:25.1_0": "Sotthiko ceva rambho ca,",
  "bv23:25.2_0": "ahesuṁ aggupaṭṭhakā;",
  "bv23:25.3_0": "Gotamī sirimā ceva,",
  "bv23:25.4_0": "ahesuṁ aggupaṭṭhikā.",
  "bv23:26.1_0": "Saṭṭhiratanamubbedho,",
  "bv23:26.2_0": "hemayūpasamūpamo;",
  "bv23:26.3_0": "Kāyā niccharati rasmi,",
  "bv23:26.4_0": "rattiṁva pabbate sikhī.",
  "bv23:27.1_0": "Saṭṭhivassasahassāni,",
  "bv23:27.2_0": "āyu tassa mahesino;",
  "bv23:27.3_0": "Tāvatā tiṭṭhamāno so,",
  "bv23:27.4_0": "tāresi janataṁ bahuṁ.",
  "bv23:28.1_0": "Dhammaṁ vitthārikaṁ katvā,",
  "bv23:28.2_0": "vibhajitvā mahājanaṁ;",
  "bv23:28.3_0": "Dhammanāvaṁ ṭhapetvāna,",
  "bv23:28.4_0": "nibbuto so sasāvako.",
  "bv23:29.1_0": "Dassaneyyaṁ sabbajanaṁ,",
  "bv23:29.2_0": "vihāraṁ iriyāpathaṁ;",
  "bv23:29.3_0": "Sabbaṁ tamantarahitaṁ,",
  "bv23:29.4_0": "nanu rittā sabbasaṅkhārā.",
  "bv23:30.1_0": "Vessabhū jinavaro satthā,",
  "bv23:30.2_0": "Khemārāmamhi nibbuto;",
  "bv23:30.3_0": "Dhātuvitthārikaṁ āsi,",
  "bv23:30.4_0": "Tesu tesu padesato”ti. ",
  "bv23:31.1_0": "Vessabhussa bhagavato vaṁso ekavīsatimo."
}