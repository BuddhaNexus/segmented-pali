{
  "ja269:0.1_0": "Jātaka",
  "ja269:0.2_0": "Tikanipāta",
  "ja269:0.3_0": "Padumavagga",
  "ja269:0.4_0": "9. Sujātajātaka",
  "ja269:1.1_0": "“Na hi vaṇṇena sampannā,",
  "ja269:1.2_0": "mañjukā piyadassanā;",
  "ja269:1.3_0": "Kharavācā piyā hoti,",
  "ja269:1.4_0": "asmiṁ loke paramhi ca.",
  "ja269:2.1_0": "Nanu passasimaṁ kāḷiṁ,",
  "ja269:2.2_0": "dubbaṇṇaṁ tilakāhataṁ;",
  "ja269:2.3_0": "Kokilaṁ saṇhavācena,",
  "ja269:2.4_0": "bahūnaṁ pāṇinaṁ piyaṁ.",
  "ja269:3.1_0": "Tasmā sakhilavācassa,",
  "ja269:3.2_0": "mantabhāṇī anuddhato;",
  "ja269:3.3_0": "Atthaṁ dhammañca dīpeti,",
  "ja269:3.4_0": "madhuraṁ tassa bhāsitan”ti.",
  "ja269:4.1_0": "Sujātajātakaṁ navamaṁ."
}