{
  "thag3.14:0.1_0": "Theragāthā",
  "thag3.14:0.2_0": "Tikanipāta",
  "thag3.14:0.3_0": "Paṭhamavagga",
  "thag3.14:0.4_0": "3.14. Gotamattheragāthā",
  "thag3.14:1.1_0": "“Saṁsaraṁ hi nirayaṁ agacchissaṁ,",
  "thag3.14:1.2_0": "Petalokamagamaṁ punappunaṁ;",
  "thag3.14:1.3_0": "Dukkhamamhipi tiracchānayoniyaṁ,",
  "thag3.14:1.4_0": "Nekadhā hi vusitaṁ ciraṁ mayā.",
  "thag3.14:2.1_0": "Mānusopi ca bhavobhirādhito,",
  "thag3.14:2.2_0": "Saggakāyamagamaṁ sakiṁ sakiṁ;",
  "thag3.14:2.3_0": "Rūpadhātusu arūpadhātusu,",
  "thag3.14:2.4_0": "Nevasaññisu asaññisuṭṭhitaṁ.",
  "thag3.14:3.1_0": "Sambhavā suviditā asārakā,",
  "thag3.14:3.2_0": "Saṅkhatā pacalitā saderitā;",
  "thag3.14:3.3_0": "Taṁ viditvā mahamattasambhavaṁ,",
  "thag3.14:3.4_0": "Santimeva satimā samajjhagan”ti.",
  "thag3.14:3.5_0": "… Gotamo thero …."
}