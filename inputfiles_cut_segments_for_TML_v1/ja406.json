{
  "ja406:0.1_0": "Jātaka",
  "ja406:0.2_0": "Sattakanipāta",
  "ja406:0.3_0": "Gandhāravagga",
  "ja406:0.4_0": "1. Gandhārajātaka",
  "ja406:1.1_0": "“Hitvā gāmasahassāni,",
  "ja406:1.2_0": "paripuṇṇāni soḷasa;",
  "ja406:1.3_0": "Koṭṭhāgārāni phītāni,",
  "ja406:1.4_0": "sannidhiṁ dāni kubbasi”.",
  "ja406:2.1_0": "“Hitvā gandhāravisayaṁ,",
  "ja406:2.2_0": "pahūtadhanadhāriyaṁ;",
  "ja406:2.3_0": "Pasāsanato nikkhanto,",
  "ja406:2.4_0": "idha dāni pasāsasi”.",
  "ja406:3.1_0": "“Dhammaṁ bhaṇāmi vedeha,",
  "ja406:3.2_0": "adhammo me na ruccati;",
  "ja406:3.3_0": "Dhammaṁ me bhaṇamānassa,",
  "ja406:3.4_0": "na pāpamupalimpati”.",
  "ja406:4.1_0": "“Yena kenaci vaṇṇena,",
  "ja406:4.2_0": "paro labhati ruppanaṁ;",
  "ja406:4.3_0": "Mahatthiyampi ce vācaṁ,",
  "ja406:4.4_0": "na taṁ bhāseyya paṇḍito”.",
  "ja406:5.1_0": "“Kāmaṁ ruppatu vā mā vā,",
  "ja406:5.2_0": "bhusaṁva vikirīyatu;",
  "ja406:5.3_0": "Dhammaṁ me bhaṇamānassa,",
  "ja406:5.4_0": "na pāpamupalimpati”.",
  "ja406:6.1_0": "“No ce assa sakā buddhi,",
  "ja406:6.2_0": "vinayo vā susikkhito;",
  "ja406:6.3_0": "Vane andhamahiṁsova,",
  "ja406:6.4_0": "careyya bahuko jano.",
  "ja406:7.1_0": "Yasmā ca panidhekacce,",
  "ja406:7.2_0": "āceramhi susikkhitā;",
  "ja406:7.3_0": "Tasmā vinītavinayā,",
  "ja406:7.4_0": "caranti susamāhitā”ti.",
  "ja406:8.1_0": "Gandhārajātakaṁ paṭhamaṁ."
}