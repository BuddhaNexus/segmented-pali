{
  "ja535:0.1_0": "Jātaka",
  "ja535:0.2_0": "Asītinipāta",
  "ja535:0.3_0": "Cūḷahaṁsavagga",
  "ja535:0.4_0": "3. Sudhābhojanajātaka",
  "ja535:1.1_0": "“Neva kiṇāmi napi vikkiṇāmi,",
  "ja535:1.2_0": "Na cāpi me sannicayo ca atthi;",
  "ja535:1.3_0": "Sukiccharūpaṁ vatidaṁ parittaṁ,",
  "ja535:1.4_0": "Patthodano nālamayaṁ duvinnaṁ”.",
  "ja535:2.1_0": "“Appamhā appakaṁ dajjā,",
  "ja535:2.2_0": "anumajjhato majjhakaṁ;",
  "ja535:2.3_0": "Bahumhā bahukaṁ dajjā,",
  "ja535:2.4_0": "adānaṁ nupapajjati.",
  "ja535:3.1_0": "Taṁ taṁ vadāmi kosiya,",
  "ja535:3.2_0": "dehi dānāni bhuñja ca;",
  "ja535:3.3_0": "Ariyamaggaṁ samārūha,",
  "ja535:3.4_0": "nekāsī labhate sukhaṁ”.",
  "ja535:4.1_0": "“Moghañcassa hutaṁ hoti,",
  "ja535:4.2_0": "moghañcāpi samīhitaṁ;",
  "ja535:4.3_0": "Atithismiṁ yo nisinnasmiṁ,",
  "ja535:4.4_0": "eko bhuñjati bhojanaṁ.",
  "ja535:5.1_0": "Taṁ taṁ vadāmi kosiya,",
  "ja535:5.2_0": "dehi dānāni bhuñja ca;",
  "ja535:5.3_0": "Ariyamaggaṁ samārūha,",
  "ja535:5.4_0": "nekāsī labhate sukhaṁ”.",
  "ja535:6.1_0": "“Saccañcassa hutaṁ hoti,",
  "ja535:6.2_0": "saccañcāpi samīhitaṁ;",
  "ja535:6.3_0": "Atithismiṁ yo nisinnasmiṁ,",
  "ja535:6.4_0": "neko bhuñjati bhojanaṁ.",
  "ja535:7.1_0": "Taṁ taṁ vadāmi kosiya,",
  "ja535:7.2_0": "dehi dānāni bhuñja ca;",
  "ja535:7.3_0": "Ariyamaggaṁ samārūha,",
  "ja535:7.4_0": "nekāsī labhate sukhaṁ”.",
  "ja535:8.1_0": "“Sarañca juhati poso,",
  "ja535:8.2_0": "bahukāya gayāya ca;",
  "ja535:8.3_0": "Doṇe timbarutitthasmiṁ,",
  "ja535:8.4_0": "sīghasote mahāvahe.",
  "ja535:9.1_0": "Atra cassa hutaṁ hoti,",
  "ja535:9.2_0": "atra cassa samīhitaṁ;",
  "ja535:9.3_0": "Atithismiṁ yo nisinnasmiṁ,",
  "ja535:9.4_0": "neko bhuñjati bhojanaṁ.",
  "ja535:10.1_0": "Taṁ taṁ vadāmi kosiya,",
  "ja535:10.2_0": "dehi dānāni bhuñja ca;",
  "ja535:10.3_0": "Ariyamaggaṁ samārūha,",
  "ja535:10.4_0": "nekāsī labhate sukhaṁ”.",
  "ja535:11.1_0": "“Baḷisañhi so nigilati,",
  "ja535:11.2_0": "dīghasuttaṁ sabandhanaṁ;",
  "ja535:11.3_0": "Atithismiṁ yo nisinnasmiṁ,",
  "ja535:11.4_0": "eko bhuñjati bhojanaṁ.",
  "ja535:12.1_0": "Taṁ taṁ vadāmi kosiya,",
  "ja535:12.2_0": "dehi dānāni bhuñja ca;",
  "ja535:12.3_0": "Ariyamaggaṁ samārūha,",
  "ja535:12.4_0": "nekāsī labhate sukhaṁ”.",
  "ja535:13.1_0": "“Uḷāravaṇṇā vata brāhmaṇā ime,",
  "ja535:13.2_0": "Ayañca vo sunakho kissa hetu;",
  "ja535:13.3_0": "Uccāvacaṁ vaṇṇanibhaṁ vikubbati,",
  "ja535:13.4_0": "Akkhātha no brāhmaṇā ke nu tumhe”.",
  "ja535:14.1_0": "“Cando ca suriyo ca ubho idhāgatā,",
  "ja535:14.2_0": "Ayaṁ pana mātali devasārathi;",
  "ja535:14.3_0": "Sakkohamasmi tidasānamindo,",
  "ja535:14.4_0": "Eso ca kho pañcasikhoti vuccati.",
  "ja535:15.1_0": "Pāṇissarā mudiṅgā ca,",
  "ja535:15.2_0": "murajālambarāni ca;",
  "ja535:15.3_0": "Suttamenaṁ pabodhenti,",
  "ja535:15.4_0": "paṭibuddho ca nandati.",
  "ja535:16.1_0": "Ye kecime maccharino kadariyā,",
  "ja535:16.2_0": "Paribhāsakā samaṇabrāhmaṇānaṁ;",
  "ja535:16.3_0": "Idheva nikkhippa sarīradehaṁ,",
  "ja535:16.4_0": "Kāyassa bhedā nirayaṁ vajanti.",
  "ja535:17.1_0": "Ye kecime suggatimāsamānā,",
  "ja535:17.2_0": "Dhamme ṭhitā saṁyame saṁvibhāge;",
  "ja535:17.3_0": "Idheva nikkhippa sarīradehaṁ,",
  "ja535:17.4_0": "Kāyassa bhedā sugatiṁ vajanti.",
  "ja535:18.1_0": "Tvaṁ nosi ñāti purimāsu jātisu,",
  "ja535:18.2_0": "So maccharī rosako pāpadhammo;",
  "ja535:18.3_0": "Taveva atthāya idhāgatamhā,",
  "ja535:18.4_0": "Mā pāpadhammo nirayaṁ gamittha”.",
  "ja535:19.1_0": "“Addhā maṁ vo hitakāmā,",
  "ja535:19.2_0": "yaṁ maṁ samanusāsatha;",
  "ja535:19.3_0": "Sohaṁ tathā karissāmi,",
  "ja535:19.4_0": "sabbaṁ vuttaṁ hitesibhi.",
  "ja535:20.1_0": "Esāhamajjeva upāramāmi,",
  "ja535:20.2_0": "Na cāpihaṁ kiñci kareyya pāpaṁ;",
  "ja535:20.3_0": "Na cāpi me kiñci adeyyamatthi,",
  "ja535:20.4_0": "Na cāpidatvā udakaṁ pivāmi.",
  "ja535:21.1_0": "Evañca me dadato sabbakālaṁ,",
  "ja535:21.2_0": "Bhogā ime vāsava khīyissanti;",
  "ja535:21.3_0": "Tato ahaṁ pabbajissāmi sakka,",
  "ja535:21.4_0": "Hitvāna kāmāni yathodhikāni”.",
  "ja535:22.1_0": "Naguttame girivare gandhamādane,",
  "ja535:22.2_0": "Modanti tā devavarābhipālitā;",
  "ja535:22.3_0": "Athāgamā isivaro sabbalokagū,",
  "ja535:22.4_0": "Supupphitaṁ dumavarasākhamādiya.",
  "ja535:23.1_0": "Suciṁ sugandhaṁ tidasehi sakkataṁ,",
  "ja535:23.2_0": "Pupphuttamaṁ amaravarehi sevitaṁ;",
  "ja535:23.3_0": "Aladdha maccehi va dānavehi vā,",
  "ja535:23.4_0": "Aññatra devehi tadārahaṁ hidaṁ.",
  "ja535:24.1_0": "Tato catasso kanakattacūpamā,",
  "ja535:24.2_0": "Uṭṭhāya nāriyo pamadādhipā muniṁ;",
  "ja535:24.3_0": "Āsā ca saddhā ca sirī tato hirī,",
  "ja535:24.4_0": "Iccabravuṁ nāradadeva brāhmaṇaṁ.",
  "ja535:25.1_0": "“Sace anuddiṭṭhaṁ tayā mahāmuni,",
  "ja535:25.2_0": "Pupphaṁ imaṁ pārichattassa brahme;",
  "ja535:25.3_0": "Dadāhi no sabbā gati te ijjhatu,",
  "ja535:25.4_0": "Tuvampi no hohi yatheva vāsavo”.",
  "ja535:26.1_0": "Taṁ yācamānābhisamekkha nārado,",
  "ja535:26.2_0": "Iccabravī saṅkalahaṁ udīrayi;",
  "ja535:26.3_0": "“Na mayhamatthatthi imehi koci naṁ,",
  "ja535:26.4_0": "Yāyeva vo seyyasi sā piḷandhatha”.",
  "ja535:27.1_0": "“Tvaṁ nottamevābhisamekkha nārada,",
  "ja535:27.2_0": "Yassicchasi tassā anuppavecchasu;",
  "ja535:27.3_0": "Yassā hi no nārada tvaṁ padassasi,",
  "ja535:27.4_0": "Sāyeva no hehiti seṭṭhasammatā”.",
  "ja535:28.1_0": "“Akallametaṁ vacanaṁ sugatte,",
  "ja535:28.2_0": "Ko brāhmaṇo saṅkalahaṁ udīraye;",
  "ja535:28.3_0": "Gantvāna bhūtādhipameva pucchatha,",
  "ja535:28.4_0": "Sace na jānātha idhuttamādhamaṁ”.",
  "ja535:29.1_0": "Tā nāradena paramappakopitā,",
  "ja535:29.2_0": "Udīritā vaṇṇamadena mattā;",
  "ja535:29.3_0": "Sakāse gantvāna sahassacakkhuno,",
  "ja535:29.4_0": "“Pucchiṁsu bhūtādhipaṁ kā nu seyyasi”.",
  "ja535:30.1_0": "Tā disvā āyattamanā purindado,",
  "ja535:30.2_0": "Iccabravī devavaro katañjalī;",
  "ja535:30.3_0": "“Sabbāva vo hotha sugatte sādisī,",
  "ja535:30.4_0": "Ko neva bhadde kalahaṁ udīrayi”.",
  "ja535:31.1_0": "“Yo sabbalokaccarito mahāmuni,",
  "ja535:31.2_0": "Dhamme ṭhito nārado saccanikkamo;",
  "ja535:31.3_0": "So nobravi girivare gandhamādane,",
  "ja535:31.4_0": "Gantvāna bhūtādhipameva pucchatha;",
  "ja535:31.5_0": "Sace na jānātha idhuttamādhamaṁ”.",
  "ja535:32.1_0": "“Asu brahāraññacaro mahāmuni,",
  "ja535:32.2_0": "Nādatvā bhattaṁ varagatte bhuñjati;",
  "ja535:32.3_0": "Viceyya dānāni dadāti kosiyo,",
  "ja535:32.4_0": "Yassā hi so dassati sāva seyyasi”.",
  "ja535:33.1_0": "“Asū hi yo sammati dakkhiṇaṁ disaṁ,",
  "ja535:33.2_0": "Gaṅgāya tīre himavantapassani;",
  "ja535:33.3_0": "Sa kosiyo dullabhapānabhojano,",
  "ja535:33.4_0": "Tassa sudhaṁ pāpaya devasārathi”.",
  "ja535:34.1_0": "Sa mātalī devavarena pesito,",
  "ja535:34.2_0": "Sahassayuttaṁ abhiruyha sandanaṁ;",
  "ja535:34.3_0": "Sukhippameva upagamma assamaṁ,",
  "ja535:34.4_0": "Adissamāno munino sudhaṁ adā.",
  "ja535:35.1_0": "“Udaggihuttaṁ upatiṭṭhato hi me,",
  "ja535:35.2_0": "Pabhaṅkaraṁ lokatamonuduttamaṁ;",
  "ja535:35.3_0": "Sabbāni bhūtāni adhicca vāsavo,",
  "ja535:35.4_0": "Ko neva me pāṇisu kiṁ sudhodahi.",
  "ja535:36.1_0": "Saṅkhūpamaṁ setamatulyadassanaṁ,",
  "ja535:36.2_0": "Suciṁ sugandhaṁ piyarūpamabbhutaṁ;",
  "ja535:36.3_0": "Adiṭṭhapubbaṁ mama jātu cakkhubhi,",
  "ja535:36.4_0": "Kā devatā pāṇisu kiṁ sudhodahi”.",
  "ja535:37.1_0": "“Ahaṁ mahindena mahesi pesito,",
  "ja535:37.2_0": "Sudhābhihāsiṁ turito mahāmuni;",
  "ja535:37.3_0": "Jānāsi maṁ mātali devasārathi,",
  "ja535:37.4_0": "Bhuñjassu bhattuttama mābhivārayi.",
  "ja535:38.1_0": "Bhuttā ca sā dvādasa hanti pāpake,",
  "ja535:38.2_0": "Khudaṁ pipāsaṁ aratiṁ daraklamaṁ;",
  "ja535:38.3_0": "Kodhūpanāhañca vivādapesuṇaṁ,",
  "ja535:38.4_0": "Sītuṇha tandiñca rasuttamaṁ idaṁ”.",
  "ja535:39.1_0": "“Na kappatī mātali mayha bhuñjituṁ,",
  "ja535:39.2_0": "Pubbe adatvā iti me vatuttamaṁ;",
  "ja535:39.3_0": "Na cāpi ekāsnamarīyapūjitaṁ,",
  "ja535:39.4_0": "Asaṁvibhāgī ca sukhaṁ na vindati”.",
  "ja535:40.1_0": "“Thīghātakā ye cime pāradārikā,",
  "ja535:40.2_0": "Mittadduno ye ca sapanti subbate;",
  "ja535:40.3_0": "Sabbe ca te maccharipañcamādhamā,",
  "ja535:40.4_0": "Tasmā adatvā udakampi nāsniye.",
  "ja535:41.1_0": "Sohitthiyā vā purisassa vā pana,",
  "ja535:41.2_0": "Dassāmi dānaṁ vidusampavaṇṇitaṁ;",
  "ja535:41.3_0": "Saddhā vadaññū idha vītamaccharā,",
  "ja535:41.4_0": "Bhavanti hete sucisaccasammatā”.",
  "ja535:42.1_0": "Ato matā devavarena pesitā,",
  "ja535:42.2_0": "Kaññā catasso kanakattacūpamā;",
  "ja535:42.3_0": "Āsā ca saddhā ca sirī tato hirī,",
  "ja535:42.4_0": "Taṁ assamaṁ āgamu yattha kosiyo.",
  "ja535:43.1_0": "Tā disvā sabbo paramappamodito,",
  "ja535:43.2_0": "Subhena vaṇṇena sikhārivaggino;",
  "ja535:43.3_0": "Kaññā catasso caturo catuddisā,",
  "ja535:43.4_0": "Iccabravī mātalino ca sammukhā.",
  "ja535:44.1_0": "“Purimaṁ disaṁ kā tvaṁ pabhāsi devate,",
  "ja535:44.2_0": "Alaṅkatā tāravarāva osadhī;",
  "ja535:44.3_0": "Pucchāmi taṁ kañcanavelliviggahe,",
  "ja535:44.4_0": "Ācikkha me tvaṁ katamāsi devatā”.",
  "ja535:45.1_0": "“Sirāha devī manujebhi pūjitā,",
  "ja535:45.2_0": "Apāpasattūpanisevinī sadā;",
  "ja535:45.3_0": "Sudhāvivādena tavantimāgatā,",
  "ja535:45.4_0": "Taṁ maṁ sudhāya varapañña bhājaya.",
  "ja535:46.1_0": "Yassāhamicchāmi sudhaṁ mahāmuni,",
  "ja535:46.2_0": "So sabbakāmehi naro pamodati;",
  "ja535:46.3_0": "Sirīti maṁ jānahi jūhatuttama,",
  "ja535:46.4_0": "Taṁ maṁ sudhāya varapañña bhājaya”.",
  "ja535:47.1_0": "“Sippena vijjācaraṇena buddhiyā,",
  "ja535:47.2_0": "Narā upetā paguṇā sakammunā;",
  "ja535:47.3_0": "Tayā vihīnā na labhanti kiñcanaṁ,",
  "ja535:47.4_0": "Tayidaṁ na sādhu yadidaṁ tayā kataṁ.",
  "ja535:48.1_0": "Passāmi posaṁ alasaṁ mahagghasaṁ,",
  "ja535:48.2_0": "Sudukkulīnampi arūpimaṁ naraṁ;",
  "ja535:48.3_0": "Tayānugutto siri jātimāmapi,",
  "ja535:48.4_0": "Peseti dāsaṁ viya bhogavā sukhī.",
  "ja535:49.1_0": "Taṁ taṁ asaccaṁ avibhajjaseviniṁ,",
  "ja535:49.2_0": "Jānāmi mūḷhaṁ vidurānupātiniṁ;",
  "ja535:49.3_0": "Na tādisī arahati āsanūdakaṁ,",
  "ja535:49.4_0": "Kuto sudhā gaccha na mayha ruccasi”.",
  "ja535:50.1_0": "“Kā sukkadāṭhā paṭimukkakuṇḍalā,",
  "ja535:50.2_0": "Cittaṅgadā kambuvimaṭṭhadhārinī;",
  "ja535:50.3_0": "Osittavaṇṇaṁ paridayha sobhasi,",
  "ja535:50.4_0": "Kusaggirattaṁ apiḷayha mañjariṁ.",
  "ja535:51.1_0": "Migīva bhantā saracāpadhārinā,",
  "ja535:51.2_0": "Virādhitā mandamiva udikkhasi;",
  "ja535:51.3_0": "Ko te dutīyo idha mandalocane,",
  "ja535:51.4_0": "Na bhāyasi ekikā kānane vane”.",
  "ja535:52.1_0": "“Na me dutīyo idha matthi kosiya,",
  "ja535:52.2_0": "Masakkasārappabhavamhi devatā;",
  "ja535:52.3_0": "Āsā sudhāsāya tavantimāgatā,",
  "ja535:52.4_0": "Taṁ maṁ sudhāya varapañña bhājaya”.",
  "ja535:53.1_0": "“Āsāya yanti vāṇijā dhanesino,",
  "ja535:53.2_0": "Nāvaṁ samāruyha parenti aṇṇave;",
  "ja535:53.3_0": "Te tattha sīdanti athopi ekadā,",
  "ja535:53.4_0": "Jīnādhanā enti vinaṭṭhapābhatā.",
  "ja535:54.1_0": "Āsāya khettāni kasanti kassakā,",
  "ja535:54.2_0": "Vapanti bījāni karontupāyaso;",
  "ja535:54.3_0": "Ītīnipātena avuṭṭhitāya vā,",
  "ja535:54.4_0": "Na kiñci vindanti tato phalāgamaṁ.",
  "ja535:55.1_0": "Athattakārāni karonti bhattusu,",
  "ja535:55.2_0": "Āsaṁ purakkhatvā narā sukhesino;",
  "ja535:55.3_0": "Te bhatturatthā atigāḷhitā puna,",
  "ja535:55.4_0": "Disā panassanti aladdha kiñcanaṁ.",
  "ja535:56.1_0": "Hitvāna dhaññañca dhanañca ñātake,",
  "ja535:56.2_0": "Āsāya saggādhimanā sukhesino;",
  "ja535:56.3_0": "Tapanti lūkhampi tapaṁ cirantaraṁ,",
  "ja535:56.4_0": "Kumaggamāruyha parenti duggatiṁ.",
  "ja535:57.1_0": "Āsā visaṁvādikasammatā ime,",
  "ja535:57.2_0": "Āse sudhāsaṁ vinayassu attani;",
  "ja535:57.3_0": "Na tādisī arahati āsanūdakaṁ,",
  "ja535:57.4_0": "Kuto sudhā gaccha na mayha ruccasi”.",
  "ja535:58.1_0": "“Daddallamānā yasasā yasassinī,",
  "ja535:58.2_0": "Jighaññanāmavhayanaṁ disaṁ pati;",
  "ja535:58.3_0": "Pucchāmi taṁ kañcanavelliviggahe,",
  "ja535:58.4_0": "Ācikkha me tvaṁ katamāsi devatā”.",
  "ja535:59.1_0": "“Saddhāha devī manujehi pūjitā,",
  "ja535:59.2_0": "Apāpasattūpanisevinī sadā;",
  "ja535:59.3_0": "Sudhāvivādena tavantimāgatā,",
  "ja535:59.4_0": "Taṁ maṁ sudhāya varapañña bhājaya”.",
  "ja535:60.1_0": "“Dānaṁ damaṁ cāgamathopi saṁyamaṁ,",
  "ja535:60.2_0": "Ādāya saddhāya karonti hekadā;",
  "ja535:60.3_0": "Theyyaṁ musā kūṭamathopi pesuṇaṁ,",
  "ja535:60.4_0": "Karonti heke puna viccutā tayā.",
  "ja535:61.1_0": "Bhariyāsu poso sadisīsu pekkhavā,",
  "ja535:61.2_0": "Sīlūpapannāsu patibbatāsupi;",
  "ja535:61.3_0": "Vinetvāna chandaṁ kulitthiyāsupi,",
  "ja535:61.4_0": "Karoti saddhaṁ puna kumbhadāsiyā.",
  "ja535:62.1_0": "Tvameva saddhe paradārasevinī,",
  "ja535:62.2_0": "Pāpaṁ karosi kusalampi riñcasi;",
  "ja535:62.3_0": "Na tādisī arahati āsanūdakaṁ,",
  "ja535:62.4_0": "Kuto sudhā gaccha na mayha ruccasi”.",
  "ja535:63.1_0": "“Jighaññarattiṁ aruṇasmimūhate,",
  "ja535:63.2_0": "Yā dissati uttamarūpavaṇṇinī;",
  "ja535:63.3_0": "Tathūpamā maṁ paṭibhāsi devate,",
  "ja535:63.4_0": "Ācikkha me tvaṁ katamāsi accharā.",
  "ja535:64.1_0": "Kālā nidāgheriva aggijāriva,",
  "ja535:64.2_0": "Anileritā lohitapattamālinī;",
  "ja535:64.3_0": "Kā tiṭṭhasi mandamigāvalokayaṁ,",
  "ja535:64.4_0": "Bhāsesamānāva giraṁ na muñcasi”.",
  "ja535:65.1_0": "“Hirāha devī manujehi pūjitā,",
  "ja535:65.2_0": "Apāpasattūpanisevinī sadā;",
  "ja535:65.3_0": "Sudhāvivādena tavantimāgatā,",
  "ja535:65.4_0": "Sāhaṁ na sakkomi sudhampi yācituṁ;",
  "ja535:65.5_0": "Kopīnarūpā viya yācanitthiyā”.",
  "ja535:66.1_0": "“Dhammena ñāyena sugatte lacchasi,",
  "ja535:66.2_0": "Eso hi dhammo na hi yācanā sudhā;",
  "ja535:66.3_0": "Taṁ taṁ ayācantimahaṁ nimantaye,",
  "ja535:66.4_0": "Sudhāya yañcicchasi tampi dammi te.",
  "ja535:67.1_0": "Sā tvaṁ mayā ajja sakamhi assame,",
  "ja535:67.2_0": "Nimantitā kañcanavelliviggahe;",
  "ja535:67.3_0": "Tuvañhi me sabbarasehi pūjiyā,",
  "ja535:67.4_0": "Taṁ pūjayitvāna sudhampi asniye”.",
  "ja535:68.1_0": "Sā kosiyenānumatā jutīmatā,",
  "ja535:68.2_0": "Addhā hiri rammaṁ pāvisi yassamaṁ;",
  "ja535:68.3_0": "Udakavantaṁ phalamariyapūjitaṁ,",
  "ja535:68.4_0": "Apāpasattūpanisevitaṁ sadā.",
  "ja535:69.1_0": "Rukkhaggahānā bahukettha pupphitā,",
  "ja535:69.2_0": "Ambā piyālā panasā ca kiṁsukā;",
  "ja535:69.3_0": "Sobhañjanā loddamathopi padmakā,",
  "ja535:69.4_0": "Kekā ca bhaṅgā tilakā supupphitā.",
  "ja535:70.1_0": "Sālā karerī bahukettha jambuyo,",
  "ja535:70.2_0": "Assatthanigrodhamadhukavetasā;",
  "ja535:70.3_0": "Uddālakā pāṭali sinduvārakā,",
  "ja535:70.4_0": "Manuññagandhā mucalindaketakā.",
  "ja535:71.1_0": "Hareṇukā veḷukā keṇu tindukā,",
  "ja535:71.2_0": "Sāmākanīvāramathopi cīnakā;",
  "ja535:71.3_0": "Mocā kadalī bahukettha sāliyo,",
  "ja535:71.4_0": "Pavīhayo ābhūjino ca taṇḍulā.",
  "ja535:72.1_0": "Tassevuttarapassena,",
  "ja535:72.2_0": "jātā pokkharaṇī sivā;",
  "ja535:72.3_0": "Akakkasā apabbhārā,",
  "ja535:72.4_0": "sādhu appaṭigandhikā.",
  "ja535:73.1_0": "Tattha macchā sanniratā,",
  "ja535:73.2_0": "khemino bahubhojanā;",
  "ja535:73.3_0": "Siṅgū savaṅkā saṅkulā,",
  "ja535:73.4_0": "satavaṅkā ca rohitā;",
  "ja535:73.5_0": "Āḷigaggarakākiṇṇā,",
  "ja535:73.6_0": "pāṭhīnā kākamacchakā.",
  "ja535:74.1_0": "Tattha pakkhī sanniratā,",
  "ja535:74.2_0": "khemino bahubhojanā;",
  "ja535:74.3_0": "Haṁsā koñcā mayūrā ca,",
  "ja535:74.4_0": "cakkavākā ca kukkuhā;",
  "ja535:74.5_0": "Kuṇālakā bahū citrā,",
  "ja535:74.6_0": "sikhaṇḍī jīvajīvakā.",
  "ja535:75.1_0": "Tattha pānāya māyanti,",
  "ja535:75.2_0": "nānā migagaṇā bahū;",
  "ja535:75.3_0": "Sīhā byagghā varāhā ca,",
  "ja535:75.4_0": "acchakokataracchayo.",
  "ja535:76.1_0": "Palāsādā gavajā ca,",
  "ja535:76.2_0": "mahiṁsā rohitā rurū;",
  "ja535:76.3_0": "Eṇeyyā ca varāhā ca,",
  "ja535:76.4_0": "gaṇino nīkasūkarā;",
  "ja535:76.5_0": "Kadalimigā bahukettha,",
  "ja535:76.6_0": "biḷārā sasakaṇṇikā.",
  "ja535:77.1_0": "Chamāgirī pupphavicitrasanthatā,",
  "ja535:77.2_0": "Dijābhighuṭṭhā dijasaṅghasevitā.",
  "ja535:78.1_0": "Sā suttacā nīladumābhilambitā,",
  "ja535:78.2_0": "Vijju mahāmegharivānupajjatha;",
  "ja535:78.3_0": "Tassā susambandhasiraṁ kusāmayaṁ,",
  "ja535:78.4_0": "Suciṁ sugandhaṁ ajinūpasevitaṁ;",
  "ja535:78.5_0": "Atricca kocchaṁ hirimetadabravi,",
  "ja535:78.6_0": "“Nisīda kalyāṇi sukhayidamāsanaṁ”.",
  "ja535:79.1_0": "Tassā tadā kocchagatāya kosiyo,",
  "ja535:79.2_0": "Yadicchamānāya jaṭājinandharo;",
  "ja535:79.3_0": "Navehi pattehi sayaṁ sahūdakaṁ,",
  "ja535:79.4_0": "Sudhābhihāsī turito mahāmuni.",
  "ja535:80.1_0": "Sā taṁ paṭiggayha ubhohi pāṇibhi,",
  "ja535:80.2_0": "Iccabravi attamanā jaṭādharaṁ;",
  "ja535:80.3_0": "“Handāhaṁ etarahi pūjitā tayā,",
  "ja535:80.4_0": "Gaccheyyaṁ brahme tidivaṁ jitāvinī”.",
  "ja535:81.1_0": "Sā kosiyenānumatā jutīmatā,",
  "ja535:81.2_0": "Udīritā vaṇṇamadena mattā;",
  "ja535:81.3_0": "Sakāse gantvāna sahassacakkhuno,",
  "ja535:81.4_0": "“Ayaṁ sudhā vāsava dehi me jayaṁ”.",
  "ja535:82.1_0": "Tamena sakkopi tadā apūjayi,",
  "ja535:82.2_0": "Sahindadevā surakaññamuttamaṁ;",
  "ja535:82.3_0": "Sā pañjalī devamanussapūjitā,",
  "ja535:82.4_0": "Navamhi kocchamhi yadā upāvisi.",
  "ja535:83.1_0": "Tameva saṁsī punadeva mātaliṁ,",
  "ja535:83.2_0": "Sahassanetto tidasānamindo;",
  "ja535:83.3_0": "“Gantvāna vākyaṁ mama brūhi kosiyaṁ,",
  "ja535:83.4_0": "‘Āsāya saddhā siriyā ca kosiya;",
  "ja535:83.5_0": "Hirī sudhaṁ kena malattha hetunā’”.",
  "ja535:84.1_0": "Taṁ suplavatthaṁ udatārayī rathaṁ,",
  "ja535:84.2_0": "Daddallamānaṁ upakāriyasādisaṁ;",
  "ja535:84.3_0": "Jambonadīsaṁ tapaneyyasannibhaṁ,",
  "ja535:84.4_0": "Alaṅkataṁ kañcanacittasannibhaṁ.",
  "ja535:85.1_0": "Suvaṇṇacandettha bahū nipātitā,",
  "ja535:85.2_0": "Hatthī gavāssā kikibyagghadīpiyo;",
  "ja535:85.3_0": "Eṇeyyakā laṅghamayettha pakkhino,",
  "ja535:85.4_0": "Migettha veḷuriyamayā yudhā yutā.",
  "ja535:86.1_0": "Tatthassarājaharayo ayojayuṁ,",
  "ja535:86.2_0": "Dasasatāni susunāgasādise;",
  "ja535:86.3_0": "Alaṅkate kañcanajāluracchade,",
  "ja535:86.4_0": "Āveḷine saddagame asaṅgite.",
  "ja535:87.1_0": "Taṁ yānaseṭṭhaṁ abhiruyha mātali,",
  "ja535:87.2_0": "Disā imāyo abhinādayittha;",
  "ja535:87.3_0": "Nabhañca selañca vanappatiniñca,",
  "ja535:87.4_0": "Sasāgaraṁ pabyathayittha mediniṁ.",
  "ja535:88.1_0": "Sa khippameva upagamma assamaṁ,",
  "ja535:88.2_0": "Pāvāramekaṁsakato katañjalī;",
  "ja535:88.3_0": "Bahussutaṁ vuddhaṁ vinītavantaṁ,",
  "ja535:88.4_0": "Iccabravī mātali devabrāhmaṇaṁ.",
  "ja535:89.1_0": "“Indassa vākyaṁ nisāmehi kosiya,",
  "ja535:89.2_0": "Dūto ahaṁ pucchati taṁ purindado;",
  "ja535:89.3_0": "‘Āsāya saddhā siriyā ca kosiya,",
  "ja535:89.4_0": "Hirī sudhaṁ kena malattha hetunā’”.",
  "ja535:90.1_0": "“Andhā sirī maṁ paṭibhāti mātali,",
  "ja535:90.2_0": "Saddhā aniccā pana devasārathi;",
  "ja535:90.3_0": "Āsā visaṁvādikasammatā hi me,",
  "ja535:90.4_0": "Hirī ca ariyamhi guṇe patiṭṭhitā”.",
  "ja535:91.1_0": "“Kumāriyo yācimā gottarakkhitā,",
  "ja535:91.2_0": "Jiṇṇā ca yā yā ca sabhattuitthiyo;",
  "ja535:91.3_0": "Tā chandarāgaṁ purisesu uggataṁ,",
  "ja535:91.4_0": "Hiriyā nivārenti sacittamattano.",
  "ja535:92.1_0": "Saṅgāmasīse sarasattisaṁyute,",
  "ja535:92.2_0": "Parājitānaṁ patataṁ palāyinaṁ;",
  "ja535:92.3_0": "Hiriyā nivattanti jahitva jīvitaṁ,",
  "ja535:92.4_0": "Te sampaṭicchanti punā hirīmanā.",
  "ja535:93.1_0": "Velā yathā sāgaravegavārinī,",
  "ja535:93.2_0": "Hirāya hi pāpajanaṁ nivārinī;",
  "ja535:93.3_0": "Taṁ sabbaloke hirimariyapūjitaṁ,",
  "ja535:93.4_0": "Indassa taṁ vedaya devasārathi”.",
  "ja535:94.1_0": "“Ko te imaṁ kosiya diṭṭhimodahi,",
  "ja535:94.2_0": "Brahmā mahindo atha vā pajāpati;",
  "ja535:94.3_0": "Hirāya devesu hi seṭṭhasammatā,",
  "ja535:94.4_0": "Dhītā mahindassa mahesi jāyatha”.",
  "ja535:95.1_0": "“Handehi dāni tidivaṁ apakkama,",
  "ja535:95.2_0": "Rathaṁ samāruyha mamāyitaṁ imaṁ;",
  "ja535:95.3_0": "Indo ca taṁ indasagotta kaṅkhati,",
  "ja535:95.4_0": "Ajjeva tvaṁ indasahabyataṁ vaja”.",
  "ja535:96.1_0": "“Evaṁ visujjhanti apāpakammino,",
  "ja535:96.2_0": "Atho suciṇṇassa phalaṁ na nassati;",
  "ja535:96.3_0": "Ye keci maddakkhu sudhāya bhojanaṁ,",
  "ja535:96.4_0": "Sabbeva te indasahabyataṁ gatā.",
  "ja535:97.1_0": "Hirī uppalavaṇṇāsi,",
  "ja535:97.2_0": "kosiyo dānapati bhikkhu;",
  "ja535:97.3_0": "Anuruddho pañcasikho,",
  "ja535:97.4_0": "ānando āsi mātali.",
  "ja535:98.1_0": "Sūriyo kassapo bhikkhu,",
  "ja535:98.2_0": "moggallānosi candimā;",
  "ja535:98.3_0": "Nārado sāriputtosi,",
  "ja535:98.4_0": "sambuddho āsi vāsavo”ti.",
  "ja535:99.1_0": "Sudhābhojanajātakaṁ tatiyaṁ."
}