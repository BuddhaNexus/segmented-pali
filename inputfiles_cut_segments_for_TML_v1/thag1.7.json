{
  "thag1.7:0.1_0": "Theragāthā",
  "thag1.7:0.2_0": "Ekakanipāta",
  "thag1.7:0.3_0": "Paṭhamavagga",
  "thag1.7:0.4_0": "1.7. Bhalliyattheragāthā",
  "thag1.7:1.1_0": "“Yopānudī maccurājassa senaṁ,",
  "thag1.7:1.2_0": "Naḷasetuṁva sudubbalaṁ mahogho;",
  "thag1.7:1.3_0": "Vijitāvī apetabheravo hi,",
  "thag1.7:1.4_0": "Danto so parinibbuto ṭhitatto”ti.",
  "thag1.7:1.5_0": "Itthaṁ sudaṁ āyasmā bhalliyo thero gāthaṁ abhāsitthāti."
}