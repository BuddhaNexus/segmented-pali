{
  "thag1.105:0.1_0": "Theragāthā",
  "thag1.105:0.2_0": "Ekakanipāta",
  "thag1.105:0.3_0": "Ekādasamavagga",
  "thag1.105:0.4_0": "1.105. Malitavambhattheragāthā",
  "thag1.105:1.1_0": "“Ukkaṇṭhitopi na vase,",
  "thag1.105:1.2_0": "ramamānopi pakkame;",
  "thag1.105:1.3_0": "Na tvevānatthasaṁhitaṁ,",
  "thag1.105:1.4_0": "vase vāsaṁ vicakkhaṇo”ti.",
  "thag1.105:1.5_0": "… Malitavambho thero …."
}