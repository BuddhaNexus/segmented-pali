{
  "mil3.3.13:0.1_0": "Milindapañha",
  "mil3.3.13:0.2_0": "Vicāravagga",
  "mil3.3.13:0.3_0": "13. Vitakkalakkhaṇapañha",
  "mil3.3.13:1.1_0": "“Bhante nāgasena, kiṁlakkhaṇo vitakko”ti?",
  "mil3.3.13:1.2_0": "“Appanālakkhaṇo, mahārāja, vitakko”ti.",
  "mil3.3.13:2.1_0": "“Opammaṁ karohī”ti.",
  "mil3.3.13:2.2_0": "“Yathā, mahārāja, vaḍḍhakī suparikammakataṁ dāruṁ sandhismiṁ appeti;",
  "mil3.3.13:2.3_0": "evameva kho, mahārāja, appanālakkhaṇo vitakko”ti.",
  "mil3.3.13:3.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.3.13:4.1_0": "Vitakkalakkhaṇapañho terasamo."
}