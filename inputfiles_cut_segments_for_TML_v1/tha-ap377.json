{
  "tha-ap377:0.1_0": "Therāpadāna",
  "tha-ap377:0.2_0": "Bodhivandanavagga",
  "tha-ap377:0.3_0": "5. Sattapaṇṇiyattheraapadāna",
  "tha-ap377:1.1_0": "“Sumano nāma sambuddho,",
  "tha-ap377:1.2_0": "uppajji lokanāyako;",
  "tha-ap377:1.3_0": "Pasannacitto sumano,",
  "tha-ap377:1.4_0": "sattapaṇṇimapūjayiṁ.",
  "tha-ap377:2.1_0": "Satasahassito kappe,",
  "tha-ap377:2.2_0": "Sattapaṇṇimapūjayiṁ;",
  "tha-ap377:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap377:2.4_0": "Sattapaṇṇipūjāyidaṁ phalaṁ.",
  "tha-ap377:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap377:3.2_0": "…pe…",
  "tha-ap377:3.3_0": "viharāmi anāsavo.",
  "tha-ap377:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap377:4.2_0": "…pe…",
  "tha-ap377:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap377:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap377:5.2_0": "…pe…",
  "tha-ap377:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap377:6.1_0": "Itthaṁ sudaṁ āyasmā sattapaṇṇiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap377:7.1_0": "Sattapaṇṇiyattherassāpadānaṁ pañcamaṁ."
}