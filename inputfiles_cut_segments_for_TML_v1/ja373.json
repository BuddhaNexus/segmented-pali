{
  "ja373:0.1_0": "Jātaka",
  "ja373:0.2_0": "Pañcakanipāta",
  "ja373:0.3_0": "Aḍḍhavagga",
  "ja373:0.4_0": "3. Mūsikajātaka",
  "ja373:1.1_0": "“Kuhiṁ gatā kattha gatā,",
  "ja373:1.2_0": "iti lālappatī jano;",
  "ja373:1.3_0": "Ahameveko jānāmi,",
  "ja373:1.4_0": "udapāne mūsikā hatā.",
  "ja373:2.1_0": "Yañcetaṁ iti cīti ca,",
  "ja373:2.2_0": "gadrabhova nivattasi;",
  "ja373:2.3_0": "Udapāne mūsikaṁ hantvā,",
  "ja373:2.4_0": "yavaṁ bhakkhetumicchasi.",
  "ja373:3.1_0": "Daharo cāsi dummedha,",
  "ja373:3.2_0": "paṭhamuppattiko susu;",
  "ja373:3.3_0": "Dīghañcetaṁ samāsajja,",
  "ja373:3.4_0": "na te dassāmi jīvitaṁ.",
  "ja373:4.1_0": "Nāntalikkhabhavanena,",
  "ja373:4.2_0": "nāṅgaputtapinena vā;",
  "ja373:4.3_0": "Puttena hi patthayito,",
  "ja373:4.4_0": "silokehi pamocito.",
  "ja373:5.1_0": "Sabbaṁ sutamadhīyetha,",
  "ja373:5.2_0": "hīnamukkaṭṭhamajjhimaṁ;",
  "ja373:5.3_0": "Sabbassa atthaṁ jāneyya,",
  "ja373:5.4_0": "na ca sabbaṁ payojaye;",
  "ja373:5.5_0": "Hoti tādisako kālo,",
  "ja373:5.6_0": "yattha atthāvahaṁ sutan”ti.",
  "ja373:6.1_0": "Mūsikajātakaṁ tatiyaṁ."
}