{
  "ja81:0.1_0": "Jātaka",
  "ja81:0.2_0": "Ekakanipāta",
  "ja81:0.3_0": "Apāyimhavagga",
  "ja81:0.4_0": "1. Surāpānajātaka",
  "ja81:1.1_0": "“Apāyimha anaccimha,",
  "ja81:1.2_0": "agāyimha rudimha ca;",
  "ja81:1.3_0": "Visaññikaraṇiṁ pitvā,",
  "ja81:1.4_0": "diṭṭhā nāhumha vānarā”ti.",
  "ja81:2.1_0": "Surāpānajātakaṁ paṭhamaṁ."
}