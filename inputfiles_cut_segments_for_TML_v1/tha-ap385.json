{
  "tha-ap385:0.1_0": "Therāpadāna",
  "tha-ap385:0.2_0": "Avaṭaphalavagga",
  "tha-ap385:0.3_0": "3. Udumbaraphaladāyakattheraapadāna",
  "tha-ap385:1.1_0": "“Vinatānadiyā tīre,",
  "tha-ap385:1.2_0": "vihāsi purisuttamo;",
  "tha-ap385:1.3_0": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap385:1.4_0": "ekaggaṁ susamāhitaṁ.",
  "tha-ap385:2.1_0": "Tasmiṁ pasannamānaso,",
  "tha-ap385:2.2_0": "kilesamaladhovane;",
  "tha-ap385:2.3_0": "Udumbaraphalaṁ gayha,",
  "tha-ap385:2.4_0": "buddhaseṭṭhassadāsahaṁ.",
  "tha-ap385:3.1_0": "Ekanavutito kappe,",
  "tha-ap385:3.2_0": "yaṁ phalamadadiṁ tadā;",
  "tha-ap385:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap385:3.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap385:4.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap385:4.2_0": "…pe…",
  "tha-ap385:4.3_0": "viharāmi anāsavo.",
  "tha-ap385:5.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap385:5.2_0": "…pe…",
  "tha-ap385:5.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap385:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap385:6.2_0": "…pe…",
  "tha-ap385:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap385:7.1_0": "Itthaṁ sudaṁ āyasmā udumbaraphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap385:8.1_0": "Udumbaraphaladāyakattherassāpadānaṁ tatiyaṁ."
}