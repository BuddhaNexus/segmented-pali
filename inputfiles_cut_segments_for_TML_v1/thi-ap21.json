{
  "thi-ap21:0.1_0": "Therīapadāna",
  "thi-ap21:0.2_0": "Kuṇḍalakesīvagga",
  "thi-ap21:0.3_0": "1 Kuṇḍalakesātherīapadāna",
  "thi-ap21:1.1_0": "“Padumuttaro nāma jino,",
  "thi-ap21:1.2_0": "sabbadhammāna pāragū;",
  "thi-ap21:1.3_0": "Ito satasahassamhi,",
  "thi-ap21:1.4_0": "kappe uppajji nāyako.",
  "thi-ap21:2.1_0": "Tadāhaṁ haṁsavatiyaṁ,",
  "thi-ap21:2.2_0": "jātā seṭṭhikule ahuṁ;",
  "thi-ap21:2.3_0": "Nānāratanapajjote,",
  "thi-ap21:2.4_0": "mahāsukhasamappitā.",
  "thi-ap21:3.1_0": "Upetvā taṁ mahāvīraṁ,",
  "thi-ap21:3.2_0": "assosiṁ dhammadesanaṁ;",
  "thi-ap21:3.3_0": "Tato jātappasādāhaṁ,",
  "thi-ap21:3.4_0": "upesiṁ saraṇaṁ jinaṁ.",
  "thi-ap21:4.1_0": "Tadā mahākāruṇiko,",
  "thi-ap21:4.2_0": "padumuttaranāmako;",
  "thi-ap21:4.3_0": "Khippābhiññānamagganti,",
  "thi-ap21:4.4_0": "ṭhapesi bhikkhuniṁ subhaṁ.",
  "thi-ap21:5.1_0": "Taṁ sutvā muditā hutvā,",
  "thi-ap21:5.2_0": "dānaṁ datvā mahesino;",
  "thi-ap21:5.3_0": "Nipacca sirasā pāde,",
  "thi-ap21:5.4_0": "taṁ ṭhānamabhipatthayiṁ.",
  "thi-ap21:6.1_0": "Anumodi mahāvīro,",
  "thi-ap21:6.2_0": "‘bhadde yaṁ tebhipatthitaṁ;",
  "thi-ap21:6.3_0": "Samijjhissati taṁ sabbaṁ,",
  "thi-ap21:6.4_0": "sukhinī hohi nibbutā.",
  "thi-ap21:7.1_0": "Satasahassito kappe,",
  "thi-ap21:7.2_0": "okkākakulasambhavo;",
  "thi-ap21:7.3_0": "Gotamo nāma gottena,",
  "thi-ap21:7.4_0": "satthā loke bhavissati.",
  "thi-ap21:8.1_0": "Tassa dhammesu dāyādā,",
  "thi-ap21:8.2_0": "orasā dhammanimmitā;",
  "thi-ap21:8.3_0": "Bhaddākuṇḍalakesāti,",
  "thi-ap21:8.4_0": "hessati satthu sāvikā’.",
  "thi-ap21:9.1_0": "Tena kammena sukatena,",
  "thi-ap21:9.2_0": "cetanāpaṇidhīhi ca;",
  "thi-ap21:9.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "thi-ap21:9.4_0": "tāvatiṁsamagacchahaṁ.",
  "thi-ap21:10.1_0": "Tato cutā yāmamagaṁ,",
  "thi-ap21:10.2_0": "tatohaṁ tusitaṁ gatā;",
  "thi-ap21:10.3_0": "Tato ca nimmānaratiṁ,",
  "thi-ap21:10.4_0": "vasavattipuraṁ tato.",
  "thi-ap21:11.1_0": "Yattha yatthūpapajjāmi,",
  "thi-ap21:11.2_0": "tassa kammassa vāhasā;",
  "thi-ap21:11.3_0": "Tattha tattheva rājūnaṁ,",
  "thi-ap21:11.4_0": "mahesittamakārayiṁ.",
  "thi-ap21:12.1_0": "Tato cutā manussesu,",
  "thi-ap21:12.2_0": "rājūnaṁ cakkavattinaṁ;",
  "thi-ap21:12.3_0": "Maṇḍalīnañca rājūnaṁ,",
  "thi-ap21:12.4_0": "mahesittamakārayiṁ.",
  "thi-ap21:13.1_0": "Sampattiṁ anubhotvāna,",
  "thi-ap21:13.2_0": "devesu mānusesu ca;",
  "thi-ap21:13.3_0": "Sabbattha sukhitā hutvā,",
  "thi-ap21:13.4_0": "nekakappesu saṁsariṁ.",
  "thi-ap21:14.1_0": "Imamhi bhaddake kappe,",
  "thi-ap21:14.2_0": "brahmabandhu mahāyaso;",
  "thi-ap21:14.3_0": "Kassapo nāma gottena,",
  "thi-ap21:14.4_0": "uppajji vadataṁ varo.",
  "thi-ap21:15.1_0": "Upaṭṭhāko mahesissa,",
  "thi-ap21:15.2_0": "tadā āsi narissaro;",
  "thi-ap21:15.3_0": "Kāsirājā kikī nāma,",
  "thi-ap21:15.4_0": "bārāṇasipuruttame.",
  "thi-ap21:16.1_0": "Tassa dhītā catutthāsiṁ,",
  "thi-ap21:16.2_0": "bhikkhudāyīti vissutā;",
  "thi-ap21:16.3_0": "Dhammaṁ sutvā jinaggassa,",
  "thi-ap21:16.4_0": "pabbajjaṁ samarocayiṁ.",
  "thi-ap21:17.1_0": "Anujāni na no tāto,",
  "thi-ap21:17.2_0": "agāreva tadā mayaṁ;",
  "thi-ap21:17.3_0": "Vīsavassasahassāni,",
  "thi-ap21:17.4_0": "vicarimha atanditā.",
  "thi-ap21:18.1_0": "Komāribrahmacariyaṁ,",
  "thi-ap21:18.2_0": "rājakaññā sukhedhitā;",
  "thi-ap21:18.3_0": "Buddhopaṭṭhānaniratā,",
  "thi-ap21:18.4_0": "muditā satta dhītaro.",
  "thi-ap21:19.1_0": "Samaṇī samaṇaguttā ca,",
  "thi-ap21:19.2_0": "bhikkhunī bhikkhudāyikā;",
  "thi-ap21:19.3_0": "Dhammā ceva sudhammā ca,",
  "thi-ap21:19.4_0": "sattamī saṅghadāyikā.",
  "thi-ap21:20.1_0": "Khemā uppalavaṇṇā ca,",
  "thi-ap21:20.2_0": "paṭācārā ahaṁ tadā;",
  "thi-ap21:20.3_0": "Kisāgotamī dhammadinnā,",
  "thi-ap21:20.4_0": "visākhā hoti sattamī.",
  "thi-ap21:21.1_0": "Tehi kammehi sukatehi,",
  "thi-ap21:21.2_0": "cetanāpaṇidhīhi ca;",
  "thi-ap21:21.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "thi-ap21:21.4_0": "tāvatiṁsamagacchahaṁ.",
  "thi-ap21:22.1_0": "Pacchime ca bhave dāni,",
  "thi-ap21:22.2_0": "giribbajapuruttame;",
  "thi-ap21:22.3_0": "Jātā seṭṭhikule phīte,",
  "thi-ap21:22.4_0": "yadāhaṁ yobbane ṭhitā.",
  "thi-ap21:23.1_0": "Coraṁ vadhatthaṁ nīyantaṁ,",
  "thi-ap21:23.2_0": "disvā rattā tahiṁ ahaṁ;",
  "thi-ap21:23.3_0": "Pitā me taṁ sahassena,",
  "thi-ap21:23.4_0": "mocayitvā vadhā tato.",
  "thi-ap21:24.1_0": "Adāsi tassa maṁ tāto,",
  "thi-ap21:24.2_0": "viditvāna manaṁ mama;",
  "thi-ap21:24.3_0": "Tassāhamāsiṁ visaṭṭhā,",
  "thi-ap21:24.4_0": "atīva dayitā hitā.",
  "thi-ap21:25.1_0": "So me bhūsanalobhena,",
  "thi-ap21:25.2_0": "balimajjhāsayo diso;",
  "thi-ap21:25.3_0": "Corappapātaṁ netvāna,",
  "thi-ap21:25.4_0": "pabbataṁ cetayī vadhaṁ.",
  "thi-ap21:26.1_0": "Tadāhaṁ paṇamitvāna,",
  "thi-ap21:26.2_0": "sattukaṁ sukatañjalī;",
  "thi-ap21:26.3_0": "Rakkhantī attano pāṇaṁ,",
  "thi-ap21:26.4_0": "idaṁ vacanamabraviṁ.",
  "thi-ap21:27.1_0": "‘Idaṁ suvaṇṇakeyūraṁ,",
  "thi-ap21:27.2_0": "muttā veḷuriyā bahū;",
  "thi-ap21:27.3_0": "Sabbaṁ harassu bhaddante,",
  "thi-ap21:27.4_0": "mañca dāsīti sāvaya’.",
  "thi-ap21:28.1_0": "‘Oropayassu kalyāṇī,",
  "thi-ap21:28.2_0": "mā bāḷhaṁ paridevasi;",
  "thi-ap21:28.3_0": "Na cāhaṁ abhijānāmi,",
  "thi-ap21:28.4_0": "ahantvā dhanamābhataṁ’.",
  "thi-ap21:29.1_0": "‘Yato sarāmi attānaṁ,",
  "thi-ap21:29.2_0": "yato pattosmi viññutaṁ;",
  "thi-ap21:29.3_0": "Na cāhaṁ abhijānāmi,",
  "thi-ap21:29.4_0": "aññaṁ piyataraṁ tayā.",
  "thi-ap21:30.1_0": "Ehi taṁ upagūhissaṁ,",
  "thi-ap21:30.2_0": "katvāna taṁ padakkhiṇaṁ;",
  "thi-ap21:30.3_0": "Na ca dāni puno atthi,",
  "thi-ap21:30.4_0": "mama tuyhañca saṅgamo’.",
  "thi-ap21:31.1_0": "‘Na hi sabbesu ṭhānesu,",
  "thi-ap21:31.2_0": "puriso hoti paṇḍito;",
  "thi-ap21:31.3_0": "Itthīpi paṇḍitā hoti,",
  "thi-ap21:31.4_0": "tattha tattha vicakkhaṇā.",
  "thi-ap21:32.1_0": "Na hi sabbesu ṭhānesu,",
  "thi-ap21:32.2_0": "puriso hoti paṇḍito;",
  "thi-ap21:32.3_0": "Itthīpi paṇḍitā hoti,",
  "thi-ap21:32.4_0": "lahuṁ atthavicintikā’.",
  "thi-ap21:33.1_0": "Lahuñca vata khippañca,",
  "thi-ap21:33.2_0": "nikaṭṭhe samacetayiṁ;",
  "thi-ap21:33.3_0": "Migaṁ uṇṇā yathā evaṁ,",
  "thi-ap21:33.4_0": "tadāhaṁ sattukaṁ vadhiṁ.",
  "thi-ap21:34.1_0": "Yo ca uppatitaṁ atthaṁ,",
  "thi-ap21:34.2_0": "na khippamanubujjhati;",
  "thi-ap21:34.3_0": "So haññate mandamati,",
  "thi-ap21:34.4_0": "corova girigabbhare.",
  "thi-ap21:35.1_0": "Yo ca uppatitaṁ atthaṁ,",
  "thi-ap21:35.2_0": "khippameva nibodhati;",
  "thi-ap21:35.3_0": "Muccate sattusambādhā,",
  "thi-ap21:35.4_0": "tadāhaṁ sattukā yathā.",
  "thi-ap21:36.1_0": "Tadāhaṁ pātayitvāna,",
  "thi-ap21:36.2_0": "giriduggamhi sattukaṁ;",
  "thi-ap21:36.3_0": "Santikaṁ setavatthānaṁ,",
  "thi-ap21:36.4_0": "upetvā pabbajiṁ ahaṁ.",
  "thi-ap21:37.1_0": "Saṇḍāsena ca kese me,",
  "thi-ap21:37.2_0": "luñcitvā sabbaso tadā;",
  "thi-ap21:37.3_0": "Pabbajitvāna samayaṁ,",
  "thi-ap21:37.4_0": "ācikkhiṁsu nirantaraṁ.",
  "thi-ap21:38.1_0": "Tato taṁ uggahetvāhaṁ,",
  "thi-ap21:38.2_0": "nisīditvāna ekikā;",
  "thi-ap21:38.3_0": "Samayaṁ taṁ vicintesiṁ,",
  "thi-ap21:38.4_0": "suvāno mānusaṁ karaṁ.",
  "thi-ap21:39.1_0": "Chinnaṁ gayha samīpe me,",
  "thi-ap21:39.2_0": "pātayitvā apakkami;",
  "thi-ap21:39.3_0": "Disvā nimittamalabhiṁ,",
  "thi-ap21:39.4_0": "hatthaṁ taṁ puḷavākulaṁ.",
  "thi-ap21:40.1_0": "Tato uṭṭhāya saṁviggā,",
  "thi-ap21:40.2_0": "apucchiṁ sahadhammike;",
  "thi-ap21:40.3_0": "Te avocuṁ ‘vijānanti,",
  "thi-ap21:40.4_0": "taṁ atthaṁ sakyabhikkhavo’.",
  "thi-ap21:41.1_0": "Sāhaṁ tamatthaṁ pucchissaṁ,",
  "thi-ap21:41.2_0": "upetvā buddhasāvake;",
  "thi-ap21:41.3_0": "Te mamādāya gacchiṁsu,",
  "thi-ap21:41.4_0": "buddhaseṭṭhassa santikaṁ.",
  "thi-ap21:42.1_0": "So me dhammamadesesi,",
  "thi-ap21:42.2_0": "khandhāyatanadhātuyo;",
  "thi-ap21:42.3_0": "Asubhāniccadukkhāti,",
  "thi-ap21:42.4_0": "anattāti ca nāyako.",
  "thi-ap21:43.1_0": "Tassa dhammaṁ suṇitvāhaṁ,",
  "thi-ap21:43.2_0": "dhammacakkhuṁ visodhayiṁ;",
  "thi-ap21:43.3_0": "Tato viññātasaddhammā,",
  "thi-ap21:43.4_0": "pabbajjaṁ upasampadaṁ.",
  "thi-ap21:44.1_0": "Āyācito tadā āha,",
  "thi-ap21:44.2_0": "‘ehi bhadde’ti nāyako;",
  "thi-ap21:44.3_0": "Tadāhaṁ upasampannā,",
  "thi-ap21:44.4_0": "parittaṁ toyamaddasaṁ.",
  "thi-ap21:45.1_0": "Pādapakkhālanenāhaṁ,",
  "thi-ap21:45.2_0": "ñatvā saudayabbayaṁ;",
  "thi-ap21:45.3_0": "Tathā sabbepi saṅkhāre,",
  "thi-ap21:45.4_0": "īdisaṁ cintayiṁ tadā.",
  "thi-ap21:46.1_0": "Tato cittaṁ vimucci me,",
  "thi-ap21:46.2_0": "anupādāya sabbaso;",
  "thi-ap21:46.3_0": "Khippābhiññānamaggaṁ me,",
  "thi-ap21:46.4_0": "tadā paññāpayī jino.",
  "thi-ap21:47.1_0": "Iddhīsu ca vasī homi,",
  "thi-ap21:47.2_0": "dibbāya sotadhātuyā;",
  "thi-ap21:47.3_0": "Paracittāni jānāmi,",
  "thi-ap21:47.4_0": "satthusāsanakārikā.",
  "thi-ap21:48.1_0": "Pubbenivāsaṁ jānāmi,",
  "thi-ap21:48.2_0": "dibbacakkhu visodhitaṁ;",
  "thi-ap21:48.3_0": "Khepetvā āsave sabbe,",
  "thi-ap21:48.4_0": "visuddhāsiṁ sunimmalā.",
  "thi-ap21:49.1_0": "Pariciṇṇo mayā satthā,",
  "thi-ap21:49.2_0": "kataṁ buddhassa sāsanaṁ;",
  "thi-ap21:49.3_0": "Ohito garuko bhāro,",
  "thi-ap21:49.4_0": "bhavanetti samūhatā.",
  "thi-ap21:50.1_0": "Yassatthāya pabbajitā,",
  "thi-ap21:50.2_0": "agārasmānagāriyaṁ;",
  "thi-ap21:50.3_0": "So me attho anuppatto,",
  "thi-ap21:50.4_0": "sabbasaṁyojanakkhayo.",
  "thi-ap21:51.1_0": "Atthadhammaniruttīsu,",
  "thi-ap21:51.2_0": "paṭibhāne tatheva ca;",
  "thi-ap21:51.3_0": "Ñāṇaṁ me vimalaṁ suddhaṁ,",
  "thi-ap21:51.4_0": "buddhaseṭṭhassa sāsane.",
  "thi-ap21:52.1_0": "Kilesā jhāpitā mayhaṁ,",
  "thi-ap21:52.2_0": "bhavā sabbe samūhatā;",
  "thi-ap21:52.3_0": "Nāgīva bandhanaṁ chetvā,",
  "thi-ap21:52.4_0": "viharāmi anāsavā.",
  "thi-ap21:53.1_0": "Svāgataṁ vata me āsi,",
  "thi-ap21:53.2_0": "mama buddhassa santike;",
  "thi-ap21:53.3_0": "Tisso vijjā anuppattā,",
  "thi-ap21:53.4_0": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap21:54.1_0": "Paṭisambhidā catasso,",
  "thi-ap21:54.2_0": "vimokkhāpi ca aṭṭhime;",
  "thi-ap21:54.3_0": "Chaḷabhiññā sacchikatā,",
  "thi-ap21:54.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "thi-ap21:55.1_0": "Itthaṁ sudaṁ bhaddākuṇḍalakesā bhikkhunī imā gāthāyo abhāsitthāti.",
  "thi-ap21:56.1_0": "Kuṇḍalakesātheriyāpadānaṁ paṭhamaṁ."
}