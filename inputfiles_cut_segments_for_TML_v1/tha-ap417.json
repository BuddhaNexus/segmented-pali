{
  "tha-ap417:0.1_0": "Therāpadāna",
  "tha-ap417:0.2_0": "Bhaddālivagga",
  "tha-ap417:0.3_0": "5 Nāgapallavattheraapadāna",
  "tha-ap417:1.1_0": "“Nagare bandhumatiyā,",
  "tha-ap417:1.2_0": "rājuyyāne vasāmahaṁ;",
  "tha-ap417:1.3_0": "Mama assamasāmantā,",
  "tha-ap417:1.4_0": "nisīdi lokanāyako.",
  "tha-ap417:2.1_0": "Nāgapallavamādāya,",
  "tha-ap417:2.2_0": "buddhassa abhiropayiṁ;",
  "tha-ap417:2.3_0": "Pasannacitto sumano,",
  "tha-ap417:2.4_0": "sugataṁ abhivādayiṁ.",
  "tha-ap417:3.1_0": "Ekanavutito kappe,",
  "tha-ap417:3.2_0": "yaṁ pallavamapūjayiṁ;",
  "tha-ap417:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap417:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap417:4.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap417:4.2_0": "…pe…",
  "tha-ap417:4.3_0": "viharāmi anāsavo.",
  "tha-ap417:5.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap417:5.2_0": "…pe…",
  "tha-ap417:5.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap417:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap417:6.2_0": "…pe…",
  "tha-ap417:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap417:7.1_0": "Itthaṁ sudaṁ āyasmā nāgapallavo thero imā gāthāyo abhāsitthāti.",
  "tha-ap417:8.1_0": "Nāgapallavattherassāpadānaṁ pañcamaṁ."
}