{
  "vv14:0.1_0": "Vimānavatthu",
  "vv14:0.2_0": "Itthivimāna",
  "vv14:0.3_0": "Pīṭhavagga",
  "vv14:0.4_0": "14. Dutiyasuṇisāvimānavatthu",
  "vv14:1.1_0": "“Abhikkantena vaṇṇena,",
  "vv14:1.2_0": "yā tvaṁ tiṭṭhasi devate;",
  "vv14:1.3_0": "Obhāsentī disā sabbā,",
  "vv14:1.4_0": "osadhī viya tārakā.",
  "vv14:2.1_0": "Kena tetādiso vaṇṇo,",
  "vv14:2.2_0": "kena te idha mijjhati;",
  "vv14:2.3_0": "Uppajjanti ca te bhogā,",
  "vv14:2.4_0": "ye keci manaso piyā.",
  "vv14:3.1_0": "Pucchāmi taṁ devi mahānubhāve,",
  "vv14:3.2_0": "Manussabhūtā kimakāsi puññaṁ;",
  "vv14:3.3_0": "Kenāsi evaṁ jalitānubhāvā,",
  "vv14:3.4_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv14:4.1_0": "Sā devatā attamanā,",
  "vv14:4.2_0": "moggallānena pucchitā;",
  "vv14:4.3_0": "Pañhaṁ puṭṭhā viyākāsi,",
  "vv14:4.4_0": "yassa kammassidaṁ phalaṁ.",
  "vv14:5.1_0": "“Ahaṁ manussesu manussabhūtā,",
  "vv14:5.2_0": "Suṇisā ahosiṁ sasurassa gehe.",
  "vv14:6.1_0": "Addasaṁ virajaṁ bhikkhuṁ,",
  "vv14:6.2_0": "vippasannamanāvilaṁ;",
  "vv14:6.3_0": "Tassa adāsahaṁ bhāgaṁ,",
  "vv14:6.4_0": "pasannā sehi pāṇibhi;",
  "vv14:6.5_0": "Kummāsapiṇḍaṁ datvāna,",
  "vv14:6.6_0": "modāmi nandane vane.",
  "vv14:7.1_0": "Tena metādiso vaṇṇo,",
  "vv14:7.2_0": "tena me idha mijjhati;",
  "vv14:7.3_0": "Uppajjanti ca me bhogā,",
  "vv14:7.4_0": "ye keci manaso piyā.",
  "vv14:8.1_0": "Akkhāmi te bhikkhu mahānubhāva,",
  "vv14:8.2_0": "Manussabhūtā yamakāsi puññaṁ;",
  "vv14:8.3_0": "Tenamhi evaṁ jalitānubhāvā,",
  "vv14:8.4_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv14:9.1_0": "Dutiyasuṇisāvimānaṁ cuddasamaṁ."
}