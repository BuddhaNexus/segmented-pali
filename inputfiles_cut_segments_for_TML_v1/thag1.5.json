{
  "thag1.5:0.1_0": "Theragāthā",
  "thag1.5:0.2_0": "Ekakanipāta",
  "thag1.5:0.3_0": "Paṭhamavagga",
  "thag1.5:0.4_0": "1.5. Dabbattheragāthā",
  "thag1.5:1.1_0": "“Yo duddamiyo damena danto,",
  "thag1.5:1.2_0": "Dabbo santusito vitiṇṇakaṅkho;",
  "thag1.5:1.3_0": "Vijitāvī apetabheravo hi,",
  "thag1.5:1.4_0": "Dabbo so parinibbuto ṭhitatto”ti.",
  "thag1.5:1.5_0": "Itthaṁ sudaṁ āyasmā dabbo thero gāthaṁ abhāsitthāti."
}