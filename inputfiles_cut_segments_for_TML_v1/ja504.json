{
  "ja504:0.1_0": "Jātaka",
  "ja504:0.2_0": "Vīsatinipāta",
  "ja504:0.3_0": "Mātaṅgavagga",
  "ja504:0.4_0": "8. Bhallātiyajātaka",
  "ja504:1.1_0": "Bhallātiyo nāma ahosi rājā,",
  "ja504:1.2_0": "Raṭṭhaṁ pahāya migavaṁ acāri so;",
  "ja504:1.3_0": "Agamā girivaraṁ gandhamādanaṁ,",
  "ja504:1.4_0": "Supupphitaṁ kimpurisānuciṇṇaṁ.",
  "ja504:2.1_0": "Sāḷūrasaṅghañca nisedhayitvā,",
  "ja504:2.2_0": "Dhanuṁ kalāpañca so nikkhipitvā;",
  "ja504:2.3_0": "Upāgami vacanaṁ vattukāmo,",
  "ja504:2.4_0": "Yatthaṭṭhitā kimpurisā ahesuṁ.",
  "ja504:3.1_0": "“Himaccaye hemavatāya tīre,",
  "ja504:3.2_0": "Kimidhaṭṭhitā mantayavho abhiṇhaṁ;",
  "ja504:3.3_0": "Pucchāmi vo mānusadehavaṇṇe,",
  "ja504:3.4_0": "Kathaṁ vo jānanti manussaloke”.",
  "ja504:4.1_0": "“Mallaṁ giriṁ paṇḍarakaṁ tikūṭaṁ,",
  "ja504:4.2_0": "Sītodakā anuvicarāma najjo;",
  "ja504:4.3_0": "Migā manussāva nibhāsavaṇṇā,",
  "ja504:4.4_0": "Jānanti no kimpurisāti ludda”.",
  "ja504:5.1_0": "“Sukiccharūpaṁ paridevayavho,",
  "ja504:5.2_0": "Āliṅgito cāsi piyo piyāya;",
  "ja504:5.3_0": "Pucchāmi vo mānusadehavaṇṇe,",
  "ja504:5.4_0": "Kimidha vane rodatha appatītā.",
  "ja504:6.1_0": "Sukiccharūpaṁ paridevayavho,",
  "ja504:6.2_0": "Āliṅgito cāsi piyo piyāya;",
  "ja504:6.3_0": "Pucchāmi vo mānusadehavaṇṇe,",
  "ja504:6.4_0": "Kimidha vane vilapatha appatītā.",
  "ja504:7.1_0": "Sukiccharūpaṁ paridevayavho,",
  "ja504:7.2_0": "Āliṅgito cāsi piyo piyāya;",
  "ja504:7.3_0": "Pucchāmi vo mānusadehavaṇṇe,",
  "ja504:7.4_0": "Kimidha vane socatha appatītā”.",
  "ja504:8.1_0": "“Mayekarattaṁ vippavasimha ludda,",
  "ja504:8.2_0": "Akāmakā aññamaññaṁ sarantā;",
  "ja504:8.3_0": "Tamekarattaṁ anutappamānā,",
  "ja504:8.4_0": "Socāma ‘sā ratti punaṁ na hessati’”.",
  "ja504:9.1_0": "“Yamekarattaṁ anutappathetaṁ,",
  "ja504:9.2_0": "Dhanaṁ va naṭṭhaṁ pitaraṁ va petaṁ;",
  "ja504:9.3_0": "Pucchāmi vo mānusadehavaṇṇe,",
  "ja504:9.4_0": "Kathaṁ vinā vāsamakappayittha”.",
  "ja504:10.1_0": "“Yamimaṁ nadiṁ passasi sīghasotaṁ,",
  "ja504:10.2_0": "Nānādumacchādanaṁ selakūlaṁ;",
  "ja504:10.3_0": "Taṁ me piyo uttari vassakāle,",
  "ja504:10.4_0": "Mamañca maññaṁ anubandhatīti.",
  "ja504:11.1_0": "Ahañca aṅkolakamocināmi,",
  "ja504:11.2_0": "Atimuttakaṁ sattaliyothikañca;",
  "ja504:11.3_0": "‘Piyo ca me hehiti mālabhārī,",
  "ja504:11.4_0": "Ahañca naṁ mālinī ajjhupessaṁ’.",
  "ja504:12.1_0": "Ahañcidaṁ kuravakamocināmi,",
  "ja504:12.2_0": "Uddālakā pāṭalisindhuvārakā;",
  "ja504:12.3_0": "‘Piyo ca me hehiti mālabhārī,",
  "ja504:12.4_0": "Ahañca naṁ mālinī ajjhupessaṁ’.",
  "ja504:13.1_0": "Ahañca sālassa supupphitassa,",
  "ja504:13.2_0": "Oceyya pupphāni karomi mālaṁ;",
  "ja504:13.3_0": "‘Piyo ca me hehiti mālabhārī,",
  "ja504:13.4_0": "Ahañca naṁ mālinī ajjhupessaṁ’.",
  "ja504:14.1_0": "Ahañca sālassa supupphitassa,",
  "ja504:14.2_0": "Oceyya pupphāni karomi bhāraṁ;",
  "ja504:14.3_0": "Idañca no hehiti santharatthaṁ,",
  "ja504:14.4_0": "Yatthajjimaṁ viharissāma rattiṁ.",
  "ja504:15.1_0": "Ahañca kho agaḷuṁ candanañca,",
  "ja504:15.2_0": "Silāya piṁsāmi pamattarūpā;",
  "ja504:15.3_0": "‘Piyo ca me hehiti rositaṅgo,",
  "ja504:15.4_0": "Ahañca naṁ rositā ajjhupessaṁ’.",
  "ja504:16.1_0": "Athāgamā salilaṁ sīghasotaṁ,",
  "ja504:16.2_0": "Nudaṁ sāle salaḷe kaṇṇikāre;",
  "ja504:16.3_0": "Āpūratha tena muhuttakena,",
  "ja504:16.4_0": "Sāyaṁ nadī āsi mayā suduttarā.",
  "ja504:17.1_0": "Ubhosu tīresu mayaṁ tadā ṭhitā,",
  "ja504:17.2_0": "Sampassantā ubhayo aññamaññaṁ;",
  "ja504:17.3_0": "Sakimpi rodāma sakiṁ hasāma,",
  "ja504:17.4_0": "Kicchena no āgamā saṁvarī sā.",
  "ja504:18.1_0": "Pātova kho uggate sūriyamhi,",
  "ja504:18.2_0": "Catukkaṁ nadiṁ uttariyāna ludda;",
  "ja504:18.3_0": "Āliṅgiyā aññamaññaṁ mayaṁ ubho,",
  "ja504:18.4_0": "Sakimpi rodāma sakiṁ hasāma.",
  "ja504:19.1_0": "Tīhūnakaṁ sattasatāni ludda,",
  "ja504:19.2_0": "Yamidha mayaṁ vippavasimha pubbe;",
  "ja504:19.3_0": "Vassekimaṁ jīvitaṁ bhūmipāla,",
  "ja504:19.4_0": "Ko nīdha kantāya vinā vaseyya”.",
  "ja504:20.1_0": "“Āyuñca vo kīvatako nu samma,",
  "ja504:20.2_0": "Sacepi jānātha vadetha āyuṁ;",
  "ja504:20.3_0": "Anussavā vuḍḍhato āgamā vā,",
  "ja504:20.4_0": "Akkhātha me taṁ avikampamānā”.",
  "ja504:21.1_0": "“Āyuñca no vassasahassaṁ ludda,",
  "ja504:21.2_0": "Na cantarā pāpako atthi rogo;",
  "ja504:21.3_0": "Appañca dukkhaṁ sukhameva bhiyyo,",
  "ja504:21.4_0": "Avītarāgā vijahāma jīvitaṁ”.",
  "ja504:22.1_0": "“Idañca sutvāna amānusānaṁ,",
  "ja504:22.2_0": "Bhallātiyo ittara jīvitanti;",
  "ja504:22.3_0": "Nivattatha na migavaṁ acari,",
  "ja504:22.4_0": "Adāsi dānāni abhuñji bhoge.",
  "ja504:23.1_0": "Idañca sutvāna amānusānaṁ,",
  "ja504:23.2_0": "Sammodatha mā kalahaṁ akattha;",
  "ja504:23.3_0": "Mā vo tapī attakammāparādho,",
  "ja504:23.4_0": "Yathāpi te kimpurisekarattaṁ.",
  "ja504:24.1_0": "Idañca sutvāna amānusānaṁ,",
  "ja504:24.2_0": "Sammodatha mā vivādaṁ akattha;",
  "ja504:24.3_0": "Mā vo tapī attakammāparādho,",
  "ja504:24.4_0": "Yathāpi te kimpurisekarattaṁ”.",
  "ja504:25.1_0": "“Vividhaṁ adhimanā suṇomahaṁ,",
  "ja504:25.2_0": "Vacanapathaṁ tava atthasaṁhitaṁ;",
  "ja504:25.3_0": "Muñcaṁ giraṁ nudaseva me daraṁ,",
  "ja504:25.4_0": "Samaṇa sukhāvaha jīva me ciran”ti.",
  "ja504:26.1_0": "Bhallātiyajātakaṁ aṭṭhamaṁ."
}