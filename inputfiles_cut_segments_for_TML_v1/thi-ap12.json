{
  "thi-ap12:0.1_0": "Therīapadāna",
  "thi-ap12:0.2_0": "Ekūposathikavagga",
  "thi-ap12:0.3_0": "2 Saḷalapupphikātherīapadāna",
  "thi-ap12:1.1_0": "“Candabhāgānadītīre,",
  "thi-ap12:1.2_0": "ahosiṁ kinnarī tadā;",
  "thi-ap12:1.3_0": "Addasāhaṁ devadevaṁ,",
  "thi-ap12:1.4_0": "caṅkamantaṁ narāsabhaṁ.",
  "thi-ap12:2.1_0": "Ocinitvāna saḷalaṁ,",
  "thi-ap12:2.2_0": "buddhaseṭṭhassadāsahaṁ;",
  "thi-ap12:2.3_0": "Upasiṅghi mahāvīro,",
  "thi-ap12:2.4_0": "saḷalaṁ devagandhikaṁ.",
  "thi-ap12:3.1_0": "Paṭiggahetvā sambuddho,",
  "thi-ap12:3.2_0": "vipassī lokanāyako;",
  "thi-ap12:3.3_0": "Upasiṅghi mahāvīro,",
  "thi-ap12:3.4_0": "pekkhamānāya me tadā.",
  "thi-ap12:4.1_0": "Añjaliṁ paggahetvāna,",
  "thi-ap12:4.2_0": "vanditvā dvipaduttamaṁ;",
  "thi-ap12:4.3_0": "Sakaṁ cittaṁ pasādetvā,",
  "thi-ap12:4.4_0": "tato pabbatamāruhiṁ.",
  "thi-ap12:5.1_0": "Ekanavutito kappe,",
  "thi-ap12:5.2_0": "yaṁ pupphamadadiṁ tadā;",
  "thi-ap12:5.3_0": "Duggatiṁ nābhijānāmi,",
  "thi-ap12:5.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "thi-ap12:6.1_0": "Kilesā jhāpitā mayhaṁ,",
  "thi-ap12:6.2_0": "…pe…",
  "thi-ap12:6.3_0": "viharāmi anāsavā.",
  "thi-ap12:7.1_0": "Svāgataṁ vata me āsi,",
  "thi-ap12:7.2_0": "…pe…",
  "thi-ap12:7.3_0": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap12:8.1_0": "Paṭisambhidā catasso,",
  "thi-ap12:8.2_0": "…pe…",
  "thi-ap12:8.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "thi-ap12:9.1_0": "Itthaṁ sudaṁ saḷalapupphikā bhikkhunī imā gāthāyo abhāsitthāti.",
  "thi-ap12:10.1_0": "Saḷalapupphikātheriyāpadānaṁ dutiyaṁ."
}