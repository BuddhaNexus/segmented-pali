{
  "snp4.3:0.1_0": "Sutta Nipāta 4.3",
  "snp4.3:0.2_0": "Duṭṭhaṭṭhakasutta",
  "snp4.3:1.1_0": "Vadanti ve duṭṭhamanāpi eke,",
  "snp4.3:1.2_0": "Athopi ve saccamanā vadanti;",
  "snp4.3:1.3_0": "Vādañca jātaṁ muni no upeti,",
  "snp4.3:1.4_0": "Tasmā munī natthi khilo kuhiñci.",
  "snp4.3:2.1_0": "Sakañhi diṭṭhiṁ kathamaccayeyya,",
  "snp4.3:2.2_0": "Chandānunīto ruciyā niviṭṭho;",
  "snp4.3:2.3_0": "Sayaṁ samattāni pakubbamāno,",
  "snp4.3:2.4_0": "Yathā hi jāneyya tathā vadeyya.",
  "snp4.3:3.1_0": "Yo attano sīlavatāni jantu,",
  "snp4.3:3.2_0": "Anānupuṭṭhova paresa pāva;",
  "snp4.3:3.3_0": "Anariyadhammaṁ kusalā tamāhu,",
  "snp4.3:3.4_0": "Yo ātumānaṁ sayameva pāva.",
  "snp4.3:4.1_0": "Santo ca bhikkhu abhinibbutatto,",
  "snp4.3:4.2_0": "Itihanti sīlesu akatthamāno;",
  "snp4.3:4.3_0": "Tamariyadhammaṁ kusalā vadanti,",
  "snp4.3:4.4_0": "Yassussadā natthi kuhiñci loke.",
  "snp4.3:5.1_0": "Pakappitā saṅkhatā yassa dhammā,",
  "snp4.3:5.2_0": "Purakkhatā santi avīvadātā;",
  "snp4.3:5.3_0": "Yadattani passati ānisaṁsaṁ,",
  "snp4.3:5.4_0": "Taṁ nissito kuppapaṭiccasantiṁ.",
  "snp4.3:6.1_0": "Diṭṭhīnivesā na hi svātivattā,",
  "snp4.3:6.2_0": "Dhammesu niccheyya samuggahītaṁ;",
  "snp4.3:6.3_0": "Tasmā naro tesu nivesanesu,",
  "snp4.3:6.4_0": "Nirassatī ādiyatī ca dhammaṁ.",
  "snp4.3:7.1_0": "Dhonassa hi natthi kuhiñci loke,",
  "snp4.3:7.2_0": "Pakappitā diṭṭhi bhavābhavesu;",
  "snp4.3:7.3_0": "Māyañca mānañca pahāya dhono,",
  "snp4.3:7.4_0": "Sa kena gaccheyya anūpayo so.",
  "snp4.3:8.1_0": "Upayo hi dhammesu upeti vādaṁ,",
  "snp4.3:8.2_0": "Anūpayaṁ kena kathaṁ vadeyya;",
  "snp4.3:8.3_0": "Attā nirattā na hi tassa atthi,",
  "snp4.3:8.4_0": "Adhosi so diṭṭhimidheva sabbanti.",
  "snp4.3:8.5_0": "Duṭṭhaṭṭhakasuttaṁ tatiyaṁ."
}