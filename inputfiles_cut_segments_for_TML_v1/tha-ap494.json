{
  "tha-ap494:0.1_0": "Therāpadāna",
  "tha-ap494:0.2_0": "Kiṅkaṇipupphavagga",
  "tha-ap494:0.3_0": "2. Paṁsukūlapūjakattheraapadāna",
  "tha-ap494:1.1_0": "“Himavantassāvidūre,",
  "tha-ap494:1.2_0": "udaṅgaṇo nāma pabbato;",
  "tha-ap494:1.3_0": "Tatthaddasaṁ paṁsukūlaṁ,",
  "tha-ap494:1.4_0": "dumaggamhi vilambitaṁ.",
  "tha-ap494:2.1_0": "Tīṇi kiṅkaṇipupphāni,",
  "tha-ap494:2.2_0": "ocinitvānahaṁ tadā;",
  "tha-ap494:2.3_0": "Haṭṭho haṭṭhena cittena,",
  "tha-ap494:2.4_0": "paṁsukūlamapūjayiṁ.",
  "tha-ap494:3.1_0": "Tena kammena sukatena,",
  "tha-ap494:3.2_0": "cetanāpaṇidhīhi ca;",
  "tha-ap494:3.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "tha-ap494:3.4_0": "tāvatiṁsamagacchahaṁ.",
  "tha-ap494:4.1_0": "Ekanavutito kappe,",
  "tha-ap494:4.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap494:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap494:4.4_0": "pūjitvā arahaddhajaṁ.",
  "tha-ap494:5.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap494:5.2_0": "…pe…",
  "tha-ap494:5.3_0": "viharāmi anāsavo.",
  "tha-ap494:6.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap494:6.2_0": "…pe…",
  "tha-ap494:6.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap494:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap494:7.2_0": "…pe…",
  "tha-ap494:7.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap494:8.1_0": "Itthaṁ sudaṁ āyasmā paṁsukūlapūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap494:9.1_0": "Paṁsukūlapūjakattherassāpadānaṁ dutiyaṁ."
}