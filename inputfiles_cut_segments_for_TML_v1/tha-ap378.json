{
  "tha-ap378:0.1_0": "Therāpadāna",
  "tha-ap378:0.2_0": "Bodhivandanavagga",
  "tha-ap378:0.3_0": "6. Gandhamuṭṭhiyattheraapadāna",
  "tha-ap378:1.1_0": "“Citake karīyamāne,",
  "tha-ap378:1.2_0": "nānāgandhe samāhate;",
  "tha-ap378:1.3_0": "Pasannacitto sumano,",
  "tha-ap378:1.4_0": "gandhamuṭṭhimapūjayiṁ.",
  "tha-ap378:2.1_0": "Satasahassito kappe,",
  "tha-ap378:2.2_0": "citakaṁ yaṁ apūjayiṁ;",
  "tha-ap378:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap378:2.4_0": "citapūjāyidaṁ phalaṁ.",
  "tha-ap378:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap378:3.2_0": "…pe…",
  "tha-ap378:3.3_0": "viharāmi anāsavo.",
  "tha-ap378:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap378:4.2_0": "…pe…",
  "tha-ap378:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap378:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap378:5.2_0": "…pe…",
  "tha-ap378:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap378:6.1_0": "Itthaṁ sudaṁ āyasmā gandhamuṭṭhiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap378:7.1_0": "Gandhamuṭṭhiyattherassāpadānaṁ chaṭṭhaṁ."
}