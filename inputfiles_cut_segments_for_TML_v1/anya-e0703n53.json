{
  "anya-e0703n53:0_0": "Catupaññāsatima pariccheda",
  "anya-e0703n53:1_0": "Cha rājakonāma",
  "anya-e0703n53:2_0": "1.",
  "anya-e0703n53:3_0": "Katvā vikkamma bāhū’ti, nāma bhūpālasūnuto;",
  "anya-e0703n53:4_0": "Tassāṇāya pavattiṁsu, sādaraṁ sīhaḷā khilā.",
  "anya-e0703n53:5_0": "2.",
  "anya-e0703n53:6_0": "So rājā damiḷe hantuṁ, dhanaṁ sañciya’nekādhā;",
  "anya-e0703n53:7_0": "Saṅgahaṁ sevakānañca, kurumāno yathocitaṁ.",
  "anya-e0703n53:8_0": "3.",
  "anya-e0703n53:9_0": "Alaṅkārakirīṭāni, chattasīhāsanāni ca;",
  "anya-e0703n53:10_0": "Kārayitvā’bhisekatthaṁ, sacivehā’bhiyācito.",
  "anya-e0703n53:11_0": "4.",
  "anya-e0703n53:12_0": "Na yāva rājaraṭṭhassa, gahaṇaṁ hoti tāva me;",
  "anya-e0703n53:13_0": "Chattussāpanakammena, kiṁ tenā’ti vivāriya.",
  "anya-e0703n53:14_0": "5.",
  "anya-e0703n53:15_0": "Sataṁ narasahassānaṁ, saṅkaletvā mahabbalo;",
  "anya-e0703n53:16_0": "Saṅgāmārabbha kālamhi, vātarogābhipīḷito.",
  "anya-e0703n53:17_0": "6.",
  "anya-e0703n53:18_0": "Yujjhituṁ samayo neti, dvādase vacchare lahuṁ;",
  "anya-e0703n53:19_0": "Upecca devanagaraṁ, gañchidevasahabyataṁ.",
  "anya-e0703n53:20_0": "7.",
  "anya-e0703n53:21_0": "Kittināmo ca sacivo, senāpaccamadhiṭṭhito;",
  "anya-e0703n53:22_0": "Rajjatthiko dinānaṭṭha, nijāṇaṁ sampavattayi.",
  "anya-e0703n53:23_0": "8.",
  "anya-e0703n53:24_0": "Ghātetvā taṁ mahālāna-kittināmo mahabbalo;",
  "anya-e0703n53:25_0": "Patvā’bhisekaṁ bhuñjanto, desaṁ taṁ rohaṇavhayaṁ.",
  "anya-e0703n53:26_0": "9.",
  "anya-e0703n53:27_0": "Saṁvaccharamhi tatiye, coḷayuddhe parājito;",
  "anya-e0703n53:28_0": "Sahatthena sakaṁ sīsaṁ, chinditvā sahasā mari.",
  "anya-e0703n53:29_0": "10.",
  "anya-e0703n53:30_0": "Tadāpite kirīṭādi-dhanasāraṁ samādiya;",
  "anya-e0703n53:31_0": "Pesesuṁ damiḷā coḷa-mahīpālassa santikaṁ.",
  "anya-e0703n53:32_0": "11.",
  "anya-e0703n53:33_0": "Bhayā saraṭṭhaṁ hitvā’tha, duṭṭhadese vasaṁ tadā;",
  "anya-e0703n53:34_0": "Eko vikkamapaṇḍūti, vissuto patthivattajo.",
  "anya-e0703n53:35_0": "12.",
  "anya-e0703n53:36_0": "Viññātalaṅkāvuttanto, desamāgamma rohaṇaṁ;",
  "anya-e0703n53:37_0": "Kāḷatitthe vasaṁ vassa-mekaṁ rajjamakārayi.",
  "anya-e0703n53:38_0": "13.",
  "anya-e0703n53:39_0": "Rāmanvayasamubbhūto , tadāyujjha purāgato;",
  "anya-e0703n53:40_0": "Jagatī pālanāmena, vissuto bhūbhujattajo.",
  "anya-e0703n53:41_0": "14.",
  "anya-e0703n53:42_0": "Raṇe vikkamapaṇḍutaṁ, ghātāpetvā mahabbalo;",
  "anya-e0703n53:43_0": "Tato cattāri vassāni, rajjaṁ kāresi rohaṇe.",
  "anya-e0703n53:44_0": "15.",
  "anya-e0703n53:45_0": "Tampi coḷāraṇe hantvā, mahesiṁ dhītarā saha;",
  "anya-e0703n53:46_0": "Vittasārañca sākalaṁ, coḷaraṭṭhamapesayuṁ.",
  "anya-e0703n53:47_0": "16.",
  "anya-e0703n53:48_0": "Rājā parakkamo nāma, paṇḍurājasuto tato;",
  "anya-e0703n53:49_0": "Akā vassadvayaṁ coḷā, ghātesuṁ tampi yujjhiya.",
  "anya-e0703n53:50_0": "17.",
  "anya-e0703n53:51_0": "Ime bhusaṁ lobhabalābhibhūtā,",
  "anya-e0703n53:52_0": "Gatā asesā vivasā vināsaṁ;",
  "anya-e0703n53:53_0": "Iccevamaññāya sadā sapañño,",
  "anya-e0703n53:54_0": "Taṇhakkhayeyeva ratiṁ kareyya.",
  "anya-e0703n53:55_0": "Sujanappasādasaṁvegatthāya kate mahāvaṁse",
  "anya-e0703n53:56_0": "Cha rājako nāma",
  "anya-e0703n53:57_0": "Catupaññāsatimo paricchedo."
}