{
  "ja175:0.1_0": "Jātaka",
  "ja175:0.2_0": "Dukanipāta",
  "ja175:0.3_0": "Kalyāṇavagga",
  "ja175:0.4_0": "5. Ādiccupaṭṭhānajātaka",
  "ja175:1.1_0": "“Sabbesu kira bhūtesu,",
  "ja175:1.2_0": "santi sīlasamāhitā;",
  "ja175:1.3_0": "Passa sākhāmigaṁ jammaṁ,",
  "ja175:1.4_0": "ādiccamupatiṭṭhati”.",
  "ja175:2.1_0": "“Nāssa sīlaṁ vijānātha,",
  "ja175:2.2_0": "anaññāya pasaṁsatha;",
  "ja175:2.3_0": "Aggihuttañca uhannaṁ,",
  "ja175:2.4_0": "dve ca bhinnā kamaṇḍalū”ti.",
  "ja175:3.1_0": "Ādiccupaṭṭhānajātakaṁ pañcamaṁ."
}