{
  "ja482:0.1_0": "Jātaka",
  "ja482:0.2_0": "Terasakanipāta",
  "ja482:0.3_0": "Ambavagga",
  "ja482:0.4_0": "9. Rurumigarājajātaka",
  "ja482:1.1_0": "“Tassa gāmavaraṁ dammi,",
  "ja482:1.2_0": "Nāriyo ca alaṅkatā;",
  "ja482:1.3_0": "Yo metaṁ migamakkhāti,",
  "ja482:1.4_0": "Migānaṁ migamuttamaṁ”.",
  "ja482:2.1_0": "“Mayhaṁ gāmavaraṁ dehi,",
  "ja482:2.2_0": "nāriyo ca alaṅkatā;",
  "ja482:2.3_0": "Ahaṁ te migamakkhissaṁ,",
  "ja482:2.4_0": "migānaṁ migamuttamaṁ.",
  "ja482:3.1_0": "Etasmiṁ vanasaṇḍasmiṁ,",
  "ja482:3.2_0": "ambā sālā ca pupphitā;",
  "ja482:3.3_0": "Indagopakasañchannā,",
  "ja482:3.4_0": "ettheso tiṭṭhate migo”.",
  "ja482:4.1_0": "“Dhanuṁ advejjhaṁ katvāna,",
  "ja482:4.2_0": "usuṁ sannayhupāgami;",
  "ja482:4.3_0": "Migo ca disvā rājānaṁ,",
  "ja482:4.4_0": "dūrato ajjhabhāsatha.",
  "ja482:5.1_0": "Āgamehi mahārāja,",
  "ja482:5.2_0": "mā maṁ vijjhi rathesabha;",
  "ja482:5.3_0": "Ko nu te idamakkhāsi,",
  "ja482:5.4_0": "ettheso tiṭṭhate migo”.",
  "ja482:6.1_0": "“Esa pāpacaro poso,",
  "ja482:6.2_0": "samma tiṭṭhati ārakā;",
  "ja482:6.3_0": "Soyaṁ me idamakkhāsi,",
  "ja482:6.4_0": "ettheso tiṭṭhate migo”.",
  "ja482:7.1_0": "“Saccaṁ kireva māhaṁsu,",
  "ja482:7.2_0": "narā ekacciyā idha;",
  "ja482:7.3_0": "Kaṭṭhaṁ niplavitaṁ seyyo,",
  "ja482:7.4_0": "na tvevekacciyo naro”.",
  "ja482:8.1_0": "“Kiṁ nu ruru garahasi migānaṁ,",
  "ja482:8.2_0": "Kiṁ pakkhīnaṁ kiṁ pana mānusānaṁ;",
  "ja482:8.3_0": "Bhayañhi maṁ vindatinapparūpaṁ,",
  "ja482:8.4_0": "Sutvāna taṁ mānusiṁ bhāsamānaṁ”.",
  "ja482:9.1_0": "“Yamuddhariṁ vāhane vuyhamānaṁ,",
  "ja482:9.2_0": "Mahodake salile sīghasote;",
  "ja482:9.3_0": "Tatonidānaṁ bhayamāgataṁ mama,",
  "ja482:9.4_0": "Dukkho have rāja asabbhi saṅgamo”.",
  "ja482:10.1_0": "“Sohaṁ catuppattamimaṁ vihaṅgamaṁ,",
  "ja482:10.2_0": "Tanucchidaṁ hadaye ossajāmi;",
  "ja482:10.3_0": "Hanāmi taṁ mittadubbhiṁ akiccakāriṁ,",
  "ja482:10.4_0": "Yo tādisaṁ kammakataṁ na jāne”.",
  "ja482:11.1_0": "“Dhīrassa bālassa have janinda,",
  "ja482:11.2_0": "Santo vadhaṁ nappasaṁsanti jātu;",
  "ja482:11.3_0": "Kāmaṁ gharaṁ gacchatu pāpadhammo,",
  "ja482:11.4_0": "Yañcassa bhaṭṭhaṁ tadetassa dehi;",
  "ja482:11.5_0": "Ahañca te kāmakaro bhavāmi”.",
  "ja482:12.1_0": "“Addhā rurū aññataro sataṁ so,",
  "ja482:12.2_0": "Yo dubbhato mānusassa na dubbhi;",
  "ja482:12.3_0": "Kāmaṁ gharaṁ gacchatu pāpadhammo,",
  "ja482:12.4_0": "Yañcassa bhaṭṭhaṁ tadetassa dammi;",
  "ja482:12.5_0": "Ahañca te kāmacāraṁ dadāmi”.",
  "ja482:13.1_0": "“Suvijānaṁ siṅgālānaṁ,",
  "ja482:13.2_0": "sakuṇānañca vassitaṁ;",
  "ja482:13.3_0": "Manussavassitaṁ rāja,",
  "ja482:13.4_0": "dubbijānataraṁ tato.",
  "ja482:14.1_0": "Api ce maññatī poso,",
  "ja482:14.2_0": "ñāti mitto sakhāti vā;",
  "ja482:14.3_0": "Yo pubbe sumano hutvā,",
  "ja482:14.4_0": "pacchā sampajjate diso”.",
  "ja482:15.1_0": "“Samāgatā jānapadā,",
  "ja482:15.2_0": "negamā ca samāgatā;",
  "ja482:15.3_0": "Migā sassāni khādanti,",
  "ja482:15.4_0": "taṁ devo paṭisedhatu”.",
  "ja482:16.1_0": "“Kāmaṁ janapado māsi,",
  "ja482:16.2_0": "raṭṭhañcāpi vinassatu;",
  "ja482:16.3_0": "Na tvevāhaṁ ruruṁ dubbhe,",
  "ja482:16.4_0": "datvā abhayadakkhiṇaṁ.",
  "ja482:17.1_0": "Mā me janapado āsi,",
  "ja482:17.2_0": "raṭṭhañcāpi vinassatu;",
  "ja482:17.3_0": "Na tvevāhaṁ migarājassa,",
  "ja482:17.4_0": "varaṁ datvā musā bhaṇe”ti.",
  "ja482:18.1_0": "Rurumigarājajātakaṁ navamaṁ."
}