{
  "tha-ap99:0.1_0": "Therāpadāna",
  "tha-ap99:0.2_0": "Sudhāvagga",
  "tha-ap99:0.3_0": "7. Madhupiṇḍikattheraapadāna",
  "tha-ap99:1.1_0": "“Vipine kānane disvā,",
  "tha-ap99:1.2_0": "appasadde nirākule;",
  "tha-ap99:1.3_0": "Siddhatthaṁ isinaṁ seṭṭhaṁ,",
  "tha-ap99:1.4_0": "āhutīnaṁ paṭiggahaṁ.",
  "tha-ap99:2.1_0": "Nibbutattaṁ mahānāgaṁ,",
  "tha-ap99:2.2_0": "nisabhājāniyaṁ yathā;",
  "tha-ap99:2.3_0": "Osadhiṁva virocantaṁ,",
  "tha-ap99:2.4_0": "devasaṅghanamassitaṁ.",
  "tha-ap99:3.1_0": "Vitti mamāhu tāvade,",
  "tha-ap99:3.2_0": "ñāṇaṁ uppajji tāvade;",
  "tha-ap99:3.3_0": "Vuṭṭhitassa samādhimhā,",
  "tha-ap99:3.4_0": "madhuṁ datvāna satthuno.",
  "tha-ap99:4.1_0": "Vanditvā satthuno pāde,",
  "tha-ap99:4.2_0": "pakkāmiṁ pācināmukho;",
  "tha-ap99:4.3_0": "Catuttiṁsamhi kappamhi,",
  "tha-ap99:4.4_0": "rājā āsiṁ sudassano.",
  "tha-ap99:5.1_0": "Madhu bhisehi savati,",
  "tha-ap99:5.2_0": "bhojanamhi ca tāvade;",
  "tha-ap99:5.3_0": "Madhuvassaṁ pavassittha,",
  "tha-ap99:5.4_0": "pubbakammassidaṁ phalaṁ.",
  "tha-ap99:6.1_0": "Catunnavutito kappe,",
  "tha-ap99:6.2_0": "yaṁ madhuṁ adadiṁ tadā;",
  "tha-ap99:6.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap99:6.4_0": "madhudānassidaṁ phalaṁ.",
  "tha-ap99:7.1_0": "Catuttiṁse ito kappe,",
  "tha-ap99:7.2_0": "cattāro te sudassanā;",
  "tha-ap99:7.3_0": "Sattaratanasampannā,",
  "tha-ap99:7.4_0": "cakkavattī mahabbalā.",
  "tha-ap99:8.1_0": "Paṭisambhidā catasso,",
  "tha-ap99:8.2_0": "…pe…",
  "tha-ap99:8.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap99:9.1_0": "Itthaṁ sudaṁ āyasmā madhupiṇḍiko thero imā gāthāyo abhāsitthāti.",
  "tha-ap99:10.1_0": "Madhupiṇḍikattherassāpadānaṁ sattamaṁ."
}