{
  "ja87:0.1_0": "Jātaka",
  "ja87:0.2_0": "Ekakanipāta",
  "ja87:0.3_0": "Apāyimhavagga",
  "ja87:0.4_0": "7. Maṅgalajātaka",
  "ja87:1.1_0": "“Yassa maṅgalā samūhatāse,",
  "ja87:1.2_0": "Uppātā supinā ca lakkhaṇā ca;",
  "ja87:1.3_0": "So maṅgaladosavītivatto,",
  "ja87:1.4_0": "Yugayogādhigato na jātumetī”ti.",
  "ja87:2.1_0": "Maṅgalajātakaṁ sattamaṁ."
}