{
  "tha-ap477:0.1_0": "Therāpadāna",
  "tha-ap477:0.2_0": "Naḷamālivagga",
  "tha-ap477:0.3_0": "5. Kummāsadāyakattheraapadāna",
  "tha-ap477:1.1_0": "“Esanāya carantassa,",
  "tha-ap477:1.2_0": "vipassissa mahesino;",
  "tha-ap477:1.3_0": "Rittakaṁ pattaṁ disvāna,",
  "tha-ap477:1.4_0": "kummāsaṁ pūrayiṁ ahaṁ.",
  "tha-ap477:2.1_0": "Ekanavutito kappe,",
  "tha-ap477:2.2_0": "yaṁ bhikkhaṁ adadiṁ tadā;",
  "tha-ap477:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap477:2.4_0": "kummāsassa idaṁ phalaṁ.",
  "tha-ap477:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap477:3.2_0": "…pe…",
  "tha-ap477:3.3_0": "viharāmi anāsavo.",
  "tha-ap477:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap477:4.2_0": "…pe…",
  "tha-ap477:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap477:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap477:5.2_0": "…pe…",
  "tha-ap477:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap477:6.1_0": "Itthaṁ sudaṁ āyasmā kummāsadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap477:7.1_0": "Kummāsadāyakattherassāpadānaṁ pañcamaṁ."
}