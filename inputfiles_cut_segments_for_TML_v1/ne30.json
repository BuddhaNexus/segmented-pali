{
  "ne30:0.1_0": "Netti",
  "ne30:0.2_0": "Paṭiniddesavāra",
  "ne30:0.3_0": "Sampāta 11",
  "ne30:0.4_0": "Paññattihārasampāta",
  "ne30:1.1_0": "Tattha katamo paññattihārasampāto?",
  "ne30:2.1_0": "“Tasmā rakkhitacittassa, sammāsaṅkappagocaro”ti gāthā.",
  "ne30:2.2_0": "“Tasmā rakkhitacittassā”ti padaṭṭhānapaññatti satiyā.",
  "ne30:2.3_0": "“Sammāsaṅkappagocaro”ti bhāvanāpaññatti samathassa.",
  "ne30:2.4_0": "“Sammādiṭṭhipurekkhāro, ñatvāna udayabbayan”ti dassanabhūmiyā nikkhepapaññatti.",
  "ne30:2.5_0": "“Thinamiddhābhibhū bhikkhū”ti samudayassa anavasesappahānapaññatti, “sabbā duggatiyo jahe”ti bhāvanāpaññatti maggassa.",
  "ne30:2.6_0": "Niyutto paññattihārasampāto."
}