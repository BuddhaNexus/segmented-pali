{
  "tha-ap387:0.1_0": "Therāpadāna",
  "tha-ap387:0.2_0": "Avaṭaphalavagga",
  "tha-ap387:0.3_0": "5. Phārusaphaladāyakattheraapadāna",
  "tha-ap387:1.1_0": "“Suvaṇṇavaṇṇaṁ sambuddhaṁ,",
  "tha-ap387:1.2_0": "Āhutīnaṁ paṭiggahaṁ;",
  "tha-ap387:1.3_0": "Rathiyaṁ paṭipajjantaṁ,",
  "tha-ap387:1.4_0": "Phārusaphalamadāsahaṁ.",
  "tha-ap387:2.1_0": "Ekanavutito kappe,",
  "tha-ap387:2.2_0": "yaṁ phalamadadiṁ ahaṁ;",
  "tha-ap387:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap387:2.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap387:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap387:3.2_0": "…pe…",
  "tha-ap387:3.3_0": "viharāmi anāsavo.",
  "tha-ap387:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap387:4.2_0": "…pe…",
  "tha-ap387:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap387:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap387:5.2_0": "…pe…",
  "tha-ap387:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap387:6.1_0": "Itthaṁ sudaṁ āyasmā phārusaphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap387:7.1_0": "Phārusaphaladāyakattherassāpadānaṁ pañcamaṁ."
}