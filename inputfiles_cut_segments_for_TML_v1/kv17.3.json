{
  "kv17.3:0.1_0": "Kathāvatthu",
  "kv17.3:0.2_0": "Mahāpaṇṇāsaka",
  "kv17.3:0.3_0": "Sattarasamavagga",
  "kv17.3:0.4_0": "Sabbamidaṅkammatotikathā",
  "kv17.3:1.1_0": "Sabbamidaṁ kammatoti?",
  "kv17.3:1.2_0": "Āmantā.",
  "kv17.3:1.3_0": "Kammampi kammatoti?",
  "kv17.3:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv17.3:1.5_0": "sabbamidaṁ kammatoti?",
  "kv17.3:1.6_0": "Āmantā.",
  "kv17.3:1.7_0": "Sabbamidaṁ pubbekatahetūti?",
  "kv17.3:1.8_0": "Na hevaṁ vattabbe …pe…",
  "kv17.3:1.9_0": "sabbamidaṁ kammatoti?",
  "kv17.3:1.10_0": "Āmantā.",
  "kv17.3:1.11_0": "Sabbamidaṁ kammavipākatoti?",
  "kv17.3:1.12_0": "Na hevaṁ vattabbe …pe….",
  "kv17.3:2.1_0": "Sabbamidaṁ kammavipākatoti?",
  "kv17.3:2.2_0": "Āmantā.",
  "kv17.3:2.3_0": "Kammavipākena pāṇaṁ haneyyāti?",
  "kv17.3:2.4_0": "Āmantā.",
  "kv17.3:2.5_0": "Pāṇātipāto saphaloti?",
  "kv17.3:2.6_0": "Āmantā.",
  "kv17.3:2.7_0": "Kammavipāko saphaloti?",
  "kv17.3:2.8_0": "Na hevaṁ vattabbe …pe…",
  "kv17.3:2.9_0": "kammavipāko aphaloti?",
  "kv17.3:2.10_0": "Āmantā.",
  "kv17.3:2.11_0": "Pāṇātipāto aphaloti?",
  "kv17.3:2.12_0": "Na hevaṁ vattabbe …pe….",
  "kv17.3:3.1_0": "Kammavipākena adinnaṁ ādiyeyya …pe…",
  "kv17.3:3.2_0": "musā bhaṇeyya …",
  "kv17.3:3.3_0": "pisuṇaṁ bhaṇeyya …",
  "kv17.3:3.4_0": "pharusaṁ bhaṇeyya …",
  "kv17.3:3.5_0": "samphaṁ palapeyya …",
  "kv17.3:3.6_0": "sandhiṁ chindeyya …",
  "kv17.3:3.7_0": "nillopaṁ hareyya …",
  "kv17.3:3.8_0": "ekāgārikaṁ kareyya …",
  "kv17.3:3.9_0": "paripanthe tiṭṭheyya …",
  "kv17.3:3.10_0": "paradāraṁ gaccheyya …",
  "kv17.3:3.11_0": "gāmaghātakaṁ kareyya …",
  "kv17.3:3.12_0": "nigamaghātakaṁ kareyya …",
  "kv17.3:3.13_0": "kammavipākena dānaṁ dadeyya …",
  "kv17.3:3.14_0": "cīvaraṁ dadeyya …",
  "kv17.3:3.15_0": "piṇḍapātaṁ dadeyya …",
  "kv17.3:3.16_0": "senāsanaṁ dadeyya …",
  "kv17.3:3.17_0": "gilānapaccayabhesajjaparikkhāraṁ dadeyyāti?",
  "kv17.3:3.18_0": "Āmantā.",
  "kv17.3:3.19_0": "Gilānapaccayabhesajjaparikkhāro saphaloti?",
  "kv17.3:3.20_0": "Āmantā.",
  "kv17.3:3.21_0": "Kammavipāko saphaloti?",
  "kv17.3:3.22_0": "Na hevaṁ vattabbe …pe…",
  "kv17.3:3.23_0": "kammavipāko aphaloti?",
  "kv17.3:3.24_0": "Āmantā.",
  "kv17.3:3.25_0": "Gilānapaccayabhesajjaparikkhāro aphaloti?",
  "kv17.3:3.26_0": "Na hevaṁ vattabbe …pe….",
  "kv17.3:4.1_0": "Na vattabbaṁ—",
  "kv17.3:4.2_0": "“sabbamidaṁ kammato”ti?",
  "kv17.3:4.3_0": "Āmantā.",
  "kv17.3:4.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv17.3:5.1_0": "“Kammunā vattatī loko,",
  "kv17.3:5.2_0": "kammunā vattatī pajā;",
  "kv17.3:5.3_0": "Kammanibandhanā sattā,",
  "kv17.3:5.4_0": "rathassāṇīva yāyato.",
  "kv17.3:6.1_0": "Kammena kittiṁ labhate pasaṁsaṁ,",
  "kv17.3:6.2_0": "Kammena jāniñca vadhañca bandhaṁ;",
  "kv17.3:6.3_0": "Taṁ kammaṁ nānākaraṇaṁ viditvā,",
  "kv17.3:6.4_0": "Kasmā vade natthi kammanti loke”ti.",
  "kv17.3:7.1_0": "Attheva suttantoti?",
  "kv17.3:7.2_0": "Āmantā.",
  "kv17.3:7.3_0": "Tena hi “sabbamidaṁ kammato”ti.",
  "kv17.3:7.4_0": "Sabbamidaṁ kammatotikathā niṭṭhitā."
}