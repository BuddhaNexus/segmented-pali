{
  "tha-ap537:0.1_0": "Therāpadāna",
  "tha-ap537:0.2_0": "Kaccāyanavagga",
  "tha-ap537:0.3_0": "5. Kumārakassapattheraapadāna",
  "tha-ap537:1.1_0": "“Ito satasahassamhi,",
  "tha-ap537:1.2_0": "kappe uppajji nāyako;",
  "tha-ap537:1.3_0": "Sabbalokahito vīro,",
  "tha-ap537:1.4_0": "padumuttaranāmako.",
  "tha-ap537:2.1_0": "Tadāhaṁ brāhmaṇo hutvā,",
  "tha-ap537:2.2_0": "vissuto vedapāragū;",
  "tha-ap537:2.3_0": "Divāvihāraṁ vicaraṁ,",
  "tha-ap537:2.4_0": "addasaṁ lokanāyakaṁ.",
  "tha-ap537:3.1_0": "Catusaccaṁ pakāsentaṁ,",
  "tha-ap537:3.2_0": "bodhayantaṁ sadevakaṁ;",
  "tha-ap537:3.3_0": "Vicittakathikānaggaṁ,",
  "tha-ap537:3.4_0": "vaṇṇayantaṁ mahājane.",
  "tha-ap537:4.1_0": "Tadā muditacittohaṁ,",
  "tha-ap537:4.2_0": "nimantetvā tathāgataṁ;",
  "tha-ap537:4.3_0": "Nānārattehi vatthehi,",
  "tha-ap537:4.4_0": "alaṅkaritvāna maṇḍapaṁ.",
  "tha-ap537:5.1_0": "Nānāratanapajjotaṁ,",
  "tha-ap537:5.2_0": "sasaṅghaṁ bhojayiṁ tahiṁ;",
  "tha-ap537:5.3_0": "Bhojayitvāna sattāhaṁ,",
  "tha-ap537:5.4_0": "nānaggarasabhojanaṁ.",
  "tha-ap537:6.1_0": "Nānācittehi pupphehi,",
  "tha-ap537:6.2_0": "pūjayitvā sasāvakaṁ;",
  "tha-ap537:6.3_0": "Nipacca pādamūlamhi,",
  "tha-ap537:6.4_0": "taṁ ṭhānaṁ patthayiṁ ahaṁ.",
  "tha-ap537:7.1_0": "Tadā munivaro āha,",
  "tha-ap537:7.2_0": "karuṇekarasāsayo;",
  "tha-ap537:7.3_0": "‘Passathetaṁ dijavaraṁ,",
  "tha-ap537:7.4_0": "padumānanalocanaṁ.",
  "tha-ap537:8.1_0": "Pītipāmojjabahulaṁ,",
  "tha-ap537:8.2_0": "samuggatatanūruhaṁ;",
  "tha-ap537:8.3_0": "Hāsamhitavisālakkhaṁ,",
  "tha-ap537:8.4_0": "mama sāsanalālasaṁ.",
  "tha-ap537:9.1_0": "Patitaṁ pādamūle me,",
  "tha-ap537:9.2_0": "ekāvatthasumānasaṁ;",
  "tha-ap537:9.3_0": "Esa pattheti taṁ ṭhānaṁ,",
  "tha-ap537:9.4_0": "vicittakathikattanaṁ.",
  "tha-ap537:10.1_0": "Satasahassito kappe,",
  "tha-ap537:10.2_0": "okkākakulasambhavo;",
  "tha-ap537:10.3_0": "Gotamo nāma gottena,",
  "tha-ap537:10.4_0": "satthā loke bhavissati.",
  "tha-ap537:11.1_0": "Tassa dhammesu dāyādo,",
  "tha-ap537:11.2_0": "oraso dhammanimmito;",
  "tha-ap537:11.3_0": "Kumārakassapo nāma,",
  "tha-ap537:11.4_0": "hessati satthu sāvako.",
  "tha-ap537:12.1_0": "Vicittapupphadussānaṁ,",
  "tha-ap537:12.2_0": "ratanānañca vāhasā;",
  "tha-ap537:12.3_0": "Vicittakathikānaṁ so,",
  "tha-ap537:12.4_0": "aggataṁ pāpuṇissati’.",
  "tha-ap537:13.1_0": "Tena kammena sukatena,",
  "tha-ap537:13.2_0": "cetanāpaṇidhīhi ca;",
  "tha-ap537:13.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "tha-ap537:13.4_0": "tāvatiṁsamagacchahaṁ.",
  "tha-ap537:14.1_0": "Paribbhamaṁ bhavābhave,",
  "tha-ap537:14.2_0": "raṅgamajjhe yathā naṭo;",
  "tha-ap537:14.3_0": "Sākhamigatrajo hutvā,",
  "tha-ap537:14.4_0": "migiyā kucchimokkamiṁ.",
  "tha-ap537:15.1_0": "Tadā mayi kucchigate,",
  "tha-ap537:15.2_0": "vajjhavāro upaṭṭhito;",
  "tha-ap537:15.3_0": "Sākhena cattā me mātā,",
  "tha-ap537:15.4_0": "nigrodhaṁ saraṇaṁ gatā.",
  "tha-ap537:16.1_0": "Tena sā migarājena,",
  "tha-ap537:16.2_0": "maraṇā parimocitā;",
  "tha-ap537:16.3_0": "Pariccajitvā sapāṇaṁ,",
  "tha-ap537:16.4_0": "mamevaṁ ovadī tadā.",
  "tha-ap537:17.1_0": "‘Nigrodhameva seveyya,",
  "tha-ap537:17.2_0": "na sākhamupasaṁvase;",
  "tha-ap537:17.3_0": "Nigrodhasmiṁ mataṁ seyyo,",
  "tha-ap537:17.4_0": "yañce sākhamhi jīvitaṁ’.",
  "tha-ap537:18.1_0": "Tenānusiṭṭhā migayūthapena,",
  "tha-ap537:18.2_0": "Ahañca mātā ca tathetare ca;",
  "tha-ap537:18.3_0": "Āgamma rammaṁ tusitādhivāsaṁ,",
  "tha-ap537:18.4_0": "Gatā pavāsaṁ sagharaṁ yatheva.",
  "tha-ap537:19.1_0": "Puno kassapavīrassa,",
  "tha-ap537:19.2_0": "atthamentamhi sāsane;",
  "tha-ap537:19.3_0": "Āruyha selasikharaṁ,",
  "tha-ap537:19.4_0": "yuñjitvā jinasāsanaṁ.",
  "tha-ap537:20.1_0": "Idānāhaṁ rājagahe,",
  "tha-ap537:20.2_0": "jāto seṭṭhikule ahuṁ;",
  "tha-ap537:20.3_0": "Āpannasattā me mātā,",
  "tha-ap537:20.4_0": "pabbaji anagāriyaṁ.",
  "tha-ap537:21.1_0": "Sagabbhaṁ taṁ viditvāna,",
  "tha-ap537:21.2_0": "devadattamupānayuṁ;",
  "tha-ap537:21.3_0": "So avoca ‘vināsetha,",
  "tha-ap537:21.4_0": "pāpikaṁ bhikkhuniṁ imaṁ’.",
  "tha-ap537:22.1_0": "Idānipi munindena,",
  "tha-ap537:22.2_0": "jinena anukampitā;",
  "tha-ap537:22.3_0": "Sukhinī ajanī mayhaṁ,",
  "tha-ap537:22.4_0": "mātā bhikkhunupassaye.",
  "tha-ap537:23.1_0": "Taṁ viditvā mahīpālo,",
  "tha-ap537:23.2_0": "kosalo maṁ aposayi;",
  "tha-ap537:23.3_0": "Kumāraparihārena,",
  "tha-ap537:23.4_0": "nāmenāhañca kassapo.",
  "tha-ap537:24.1_0": "Mahākassapamāgamma,",
  "tha-ap537:24.2_0": "ahaṁ kumārakassapo;",
  "tha-ap537:24.3_0": "Vammikasadisaṁ kāyaṁ,",
  "tha-ap537:24.4_0": "sutvā buddhena desitaṁ.",
  "tha-ap537:25.1_0": "Tato cittaṁ vimucci me,",
  "tha-ap537:25.2_0": "anupādāya sabbaso;",
  "tha-ap537:25.3_0": "Pāyāsiṁ damayitvāhaṁ,",
  "tha-ap537:25.4_0": "etadaggamapāpuṇiṁ.",
  "tha-ap537:26.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap537:26.2_0": "…pe…",
  "tha-ap537:26.3_0": "viharāmi anāsavo.",
  "tha-ap537:27.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap537:27.2_0": "…pe…",
  "tha-ap537:27.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap537:28.1_0": "Paṭisambhidā catasso,",
  "tha-ap537:28.2_0": "…pe…",
  "tha-ap537:28.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap537:29.1_0": "Itthaṁ sudaṁ āyasmā kumārakassapo thero imā gāthāyo abhāsitthāti.",
  "tha-ap537:30.1_0": "Kumārakassapattherassāpadānaṁ pañcamaṁ.",
  "tha-ap537:30.2_0": "Catuvīsatimaṁ bhāṇavāraṁ."
}