{
  "thi-ap16:0.1_0": "Therīapadāna",
  "thi-ap16:0.2_0": "Ekūposathikavagga",
  "thi-ap16:0.3_0": "6 Naḷamālikātherīapadāna",
  "thi-ap16:1.1_0": "“Candabhāgānadītīre,",
  "thi-ap16:1.2_0": "ahosiṁ kinnarī tadā;",
  "thi-ap16:1.3_0": "Addasaṁ virajaṁ buddhaṁ,",
  "thi-ap16:1.4_0": "sayambhuṁ aparājitaṁ.",
  "thi-ap16:2.1_0": "Pasannacittā sumanā,",
  "thi-ap16:2.2_0": "vedajātā katañjalī;",
  "thi-ap16:2.3_0": "Naḷamālaṁ gahetvāna,",
  "thi-ap16:2.4_0": "sayambhuṁ abhipūjayiṁ.",
  "thi-ap16:3.1_0": "Tena kammena sukatena,",
  "thi-ap16:3.2_0": "cetanāpaṇidhīhi ca;",
  "thi-ap16:3.3_0": "Jahitvā kinnarīdehaṁ,",
  "thi-ap16:3.4_0": "tāvatiṁsamagacchahaṁ.",
  "thi-ap16:4.1_0": "Chattiṁsadevarājūnaṁ,",
  "thi-ap16:4.2_0": "mahesittamakārayiṁ;",
  "thi-ap16:4.3_0": "Manasā patthitaṁ mayhaṁ,",
  "thi-ap16:4.4_0": "nibbattati yathicchitaṁ.",
  "thi-ap16:5.1_0": "Dasannaṁ cakkavattīnaṁ,",
  "thi-ap16:5.2_0": "mahesittamakārayiṁ;",
  "thi-ap16:5.3_0": "Ocitattāva hutvāna,",
  "thi-ap16:5.4_0": "saṁsarāmi bhavesvahaṁ.",
  "thi-ap16:6.1_0": "Kusalaṁ vijjate mayhaṁ,",
  "thi-ap16:6.2_0": "pabbajiṁ anagāriyaṁ;",
  "thi-ap16:6.3_0": "Pūjārahā ahaṁ ajja,",
  "thi-ap16:6.4_0": "sakyaputtassa sāsane.",
  "thi-ap16:7.1_0": "Visuddhamanasā ajja,",
  "thi-ap16:7.2_0": "apetamanapāpikā;",
  "thi-ap16:7.3_0": "Sabbāsavā parikkhīṇā,",
  "thi-ap16:7.4_0": "natthi dāni punabbhavo.",
  "thi-ap16:8.1_0": "Catunnavutito kappe,",
  "thi-ap16:8.2_0": "yaṁ buddhamabhipūjayiṁ;",
  "thi-ap16:8.3_0": "Duggatiṁ nābhijānāmi,",
  "thi-ap16:8.4_0": "naḷamālāyidaṁ phalaṁ.",
  "thi-ap16:9.1_0": "Kilesā jhāpitā mayhaṁ,",
  "thi-ap16:9.2_0": "…pe…",
  "thi-ap16:9.3_0": "viharāmi anāsavā.",
  "thi-ap16:10.1_0": "Svāgataṁ vata me āsi,",
  "thi-ap16:10.2_0": "…pe…",
  "thi-ap16:10.3_0": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap16:11.1_0": "Paṭisambhidā catasso,",
  "thi-ap16:11.2_0": "…pe…",
  "thi-ap16:11.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "thi-ap16:12.1_0": "Itthaṁ sudaṁ naḷamālikā bhikkhunī imā gāthāyo abhāsitthāti.",
  "thi-ap16:13.1_0": "Naḷamālikātheriyāpadānaṁ chaṭṭhaṁ."
}