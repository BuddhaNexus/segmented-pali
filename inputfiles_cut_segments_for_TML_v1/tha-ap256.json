{
  "tha-ap256:0.1_0": "Therāpadāna",
  "tha-ap256:0.2_0": "Thomakavagga",
  "tha-ap256:0.3_0": "4 Ticampakapupphiyattheraapadāna",
  "tha-ap256:1.1_0": "“Himavantassāvidūre,",
  "tha-ap256:1.2_0": "vikato nāma pabbato;",
  "tha-ap256:1.3_0": "Tassa vemajjhe vasati,",
  "tha-ap256:1.4_0": "samaṇo bhāvitindriyo.",
  "tha-ap256:2.1_0": "Disvāna tassopasamaṁ,",
  "tha-ap256:2.2_0": "vippasannena cetasā;",
  "tha-ap256:2.3_0": "Tīṇi campakapupphāni,",
  "tha-ap256:2.4_0": "gahetvāna samokiriṁ.",
  "tha-ap256:3.1_0": "Ekanavutito kappe,",
  "tha-ap256:3.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap256:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap256:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap256:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap256:4.2_0": "…pe…",
  "tha-ap256:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap256:5.1_0": "Itthaṁ sudaṁ āyasmā ticampakapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap256:6.1_0": "Ticampakapupphiyattherassāpadānaṁ catutthaṁ."
}