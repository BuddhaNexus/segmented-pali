{
  "thag1.41:0.1_0": "Theragāthā",
  "thag1.41:0.2_0": "Ekakanipāta",
  "thag1.41:0.3_0": "Pañcamavagga",
  "thag1.41:0.4_0": "1.41. Sirivaḍḍhattheragāthā",
  "thag1.41:1.1_0": "“Vivaramanupatanti vijjutā,",
  "thag1.41:1.2_0": "Vebhārassa ca paṇḍavassa ca;",
  "thag1.41:1.3_0": "Nagavivaragato ca jhāyati,",
  "thag1.41:1.4_0": "Putto appaṭimassa tādino”ti.",
  "thag1.41:1.5_0": "… Sirivaḍḍho thero …."
}