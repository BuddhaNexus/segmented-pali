{
  "tha-ap76:0.1_0": "Therāpadāna",
  "tha-ap76:0.2_0": "Nāgasamālavagga",
  "tha-ap76:0.3_0": "4. Bhisāluvadāyakattheraapadāna",
  "tha-ap76:1.1_0": "“Kānanaṁ vanamogayha,",
  "tha-ap76:1.2_0": "vasāmi vipine ahaṁ;",
  "tha-ap76:1.3_0": "Vipassiṁ addasaṁ buddhaṁ,",
  "tha-ap76:1.4_0": "āhutīnaṁ paṭiggahaṁ.",
  "tha-ap76:2.1_0": "Bhisāluvañca pādāsiṁ,",
  "tha-ap76:2.2_0": "udakaṁ hatthadhovanaṁ;",
  "tha-ap76:2.3_0": "Vanditvā sirasā pāde,",
  "tha-ap76:2.4_0": "pakkāmi uttarāmukho.",
  "tha-ap76:3.1_0": "Ekanavutito kappe,",
  "tha-ap76:3.2_0": "bhisāluvamadaṁ tadā;",
  "tha-ap76:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap76:3.4_0": "puññakammassidaṁ phalaṁ.",
  "tha-ap76:4.1_0": "Ito tatiyake kappe,",
  "tha-ap76:4.2_0": "bhisasammatakhattiyo;",
  "tha-ap76:4.3_0": "Sattaratanasampanno,",
  "tha-ap76:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap76:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap76:5.2_0": "…pe…",
  "tha-ap76:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap76:6.1_0": "Itthaṁ sudaṁ āyasmā bhisāluvadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap76:7.1_0": "Bhisāluvadāyakattherassāpadānaṁ catutthaṁ.",
  "tha-ap76:7.2_0": "Chaṭṭhabhāṇavāraṁ."
}