{
  "snp2.10:0.1_0": "Sutta Nipāta 2.10",
  "snp2.10:0.2_0": "Uṭṭhānasutta",
  "snp2.10:1.1_0": "Uṭṭhahatha nisīdatha,",
  "snp2.10:1.2_0": "ko attho supitena vo;",
  "snp2.10:1.3_0": "Āturānañhi kā niddā,",
  "snp2.10:1.4_0": "sallaviddhāna ruppataṁ.",
  "snp2.10:2.1_0": "Uṭṭhahatha nisīdatha,",
  "snp2.10:2.2_0": "Daḷhaṁ sikkhatha santiyā;",
  "snp2.10:2.3_0": "Mā vo pamatte viññāya,",
  "snp2.10:2.4_0": "Maccurājā amohayittha vasānuge.",
  "snp2.10:3.1_0": "Yāya devā manussā ca,",
  "snp2.10:3.2_0": "sitā tiṭṭhanti atthikā;",
  "snp2.10:3.3_0": "Tarathetaṁ visattikaṁ,",
  "snp2.10:3.4_0": "khaṇo vo mā upaccagā;",
  "snp2.10:3.5_0": "Khaṇātītā hi socanti,",
  "snp2.10:3.6_0": "nirayamhi samappitā.",
  "snp2.10:4.1_0": "Pamādo rajo pamādo,",
  "snp2.10:4.2_0": "Pamādānupatito rajo;",
  "snp2.10:4.3_0": "Appamādena vijjāya,",
  "snp2.10:4.4_0": "Abbahe sallamattanoti.",
  "snp2.10:4.5_0": "Uṭṭhānasuttaṁ dasamaṁ."
}