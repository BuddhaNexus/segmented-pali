{
  "vv79:0.1_0": "Vimānavatthu",
  "vv79:0.2_0": "Purisavimāna",
  "vv79:0.3_0": "Sunikkhittavagga",
  "vv79:0.4_0": "5. Ambavimānavatthu",
  "vv79:1.1_0": "“Uccamidaṁ maṇithūṇaṁ vimānaṁ,",
  "vv79:1.2_0": "Samantato dvādasa yojanāni;",
  "vv79:1.3_0": "Kūṭāgārā sattasatā uḷārā,",
  "vv79:1.4_0": "Veḷuriyathambhā rucakatthatā subhā.",
  "vv79:2.1_0": "Tatthacchasi pivasi khādasi ca,",
  "vv79:2.2_0": "Dibbā ca vīṇā pavadanti vagguṁ;",
  "vv79:2.3_0": "Dibbā rasā kāmaguṇettha pañca,",
  "vv79:2.4_0": "Nāriyo ca naccanti suvaṇṇachannā.",
  "vv79:3.1_0": "Kena tetādiso vaṇṇo,",
  "vv79:3.2_0": "…pe…",
  "vv79:3.3_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv79:4.1_0": "So devaputto attamano,",
  "vv79:4.2_0": "…pe…",
  "vv79:4.3_0": "yassa kammassidaṁ phalaṁ.",
  "vv79:5.1_0": "“Gimhānaṁ pacchime māse,",
  "vv79:5.2_0": "patapante divaṅkare;",
  "vv79:5.3_0": "Paresaṁ bhatako poso,",
  "vv79:5.4_0": "ambārāmamasiñcati.",
  "vv79:6.1_0": "Atha tenāgamā bhikkhu,",
  "vv79:6.2_0": "sāriputtoti vissuto;",
  "vv79:6.3_0": "Kilantarūpo kāyena,",
  "vv79:6.4_0": "akilantova cetasā.",
  "vv79:7.1_0": "Tañca disvāna āyantaṁ,",
  "vv79:7.2_0": "Avocaṁ ambasiñcako;",
  "vv79:7.3_0": "Sādhu taṁ bhante nhāpeyyaṁ,",
  "vv79:7.4_0": "Yaṁ mamassa sukhāvahaṁ.",
  "vv79:8.1_0": "Tassa me anukampāya,",
  "vv79:8.2_0": "nikkhipi pattacīvaraṁ;",
  "vv79:8.3_0": "Nisīdi rukkhamūlasmiṁ,",
  "vv79:8.4_0": "chāyāya ekacīvaro.",
  "vv79:9.1_0": "Tañca acchena vārinā,",
  "vv79:9.2_0": "pasannamānaso naro;",
  "vv79:9.3_0": "Nhāpayī rukkhamūlasmiṁ,",
  "vv79:9.4_0": "chāyāya ekacīvaraṁ.",
  "vv79:10.1_0": "Ambo ca sitto samaṇo ca nhāpito,",
  "vv79:10.2_0": "Mayā ca puññaṁ pasutaṁ anappakaṁ;",
  "vv79:10.3_0": "Iti so pītiyā kāyaṁ,",
  "vv79:10.4_0": "Sabbaṁ pharati attano.",
  "vv79:11.1_0": "Tadeva ettakaṁ kammaṁ,",
  "vv79:11.2_0": "akāsiṁ tāya jātiyā;",
  "vv79:11.3_0": "Pahāya mānusaṁ dehaṁ,",
  "vv79:11.4_0": "upapannomhi nandanaṁ.",
  "vv79:12.1_0": "Nandane ca vane ramme,",
  "vv79:12.2_0": "nānādijagaṇāyute;",
  "vv79:12.3_0": "Ramāmi naccagītehi,",
  "vv79:12.4_0": "accharāhi purakkhato”ti.",
  "vv79:13.1_0": "Ambavimānaṁ pañcamaṁ."
}