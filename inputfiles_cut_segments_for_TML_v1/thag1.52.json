{
  "thag1.52:0.1_0": "Theragāthā",
  "thag1.52:0.2_0": "Ekakanipāta",
  "thag1.52:0.3_0": "Chaṭṭhavagga",
  "thag1.52:0.4_0": "1.52. Subāhuttheragāthā",
  "thag1.52:1.1_0": "“Vassati devo yathā sugītaṁ,",
  "thag1.52:1.2_0": "Channā me kuṭikā sukhā nivātā;",
  "thag1.52:1.3_0": "Cittaṁ susamāhitañca kāye,",
  "thag1.52:1.4_0": "Atha ce patthayasi pavassa devā”ti.",
  "thag1.52:1.5_0": "… Subāhutthero …."
}