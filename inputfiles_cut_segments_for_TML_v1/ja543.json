{
  "ja543:0.1_0": "Jātaka",
  "ja543:0.2_0": "Mahānipāta",
  "ja543:0.3_0": "Mūgapakkhavagga",
  "ja543:0.4_0": "6. Bhūridattajātaka",
  "ja543:1.1_0": "“Yaṁ kiñci ratanaṁ atthi,",
  "ja543:1.2_0": "dhataraṭṭhanivesane;",
  "ja543:1.3_0": "Sabbāni te upayantu,",
  "ja543:1.4_0": "dhītaraṁ dehi rājino”.",
  "ja543:2.1_0": "“Na no vivāho nāgehi,",
  "ja543:2.2_0": "katapubbo kudācanaṁ;",
  "ja543:2.3_0": "Taṁ vivāhaṁ asaṁyuttaṁ,",
  "ja543:2.4_0": "kathaṁ amhe karomase”.",
  "ja543:3.1_0": "“Jīvitaṁ nūna te cattaṁ,",
  "ja543:3.2_0": "raṭṭhaṁ vā manujādhipa;",
  "ja543:3.3_0": "Na hi nāge kupitamhi,",
  "ja543:3.4_0": "ciraṁ jīvanti tādisā.",
  "ja543:4.1_0": "Yo tvaṁ deva manussosi,",
  "ja543:4.2_0": "iddhimantaṁ aniddhimā;",
  "ja543:4.3_0": "Varuṇassa niyaṁputtaṁ,",
  "ja543:4.4_0": "yāmunaṁ atimaññasi”.",
  "ja543:5.1_0": "“Nātimaññāmi rājānaṁ,",
  "ja543:5.2_0": "dhataraṭṭhaṁ yasassinaṁ;",
  "ja543:5.3_0": "Dhataraṭṭho hi nāgānaṁ,",
  "ja543:5.4_0": "bahūnamapi issaro.",
  "ja543:6.1_0": "Ahi mahānubhāvopi,",
  "ja543:6.2_0": "na me dhītaramāraho;",
  "ja543:6.3_0": "Khattiyo ca videhānaṁ,",
  "ja543:6.4_0": "abhijātā samuddajā”.",
  "ja543:7.1_0": "“Kambalassatarā uṭṭhentu,",
  "ja543:7.2_0": "sabbe nāge nivedaya;",
  "ja543:7.3_0": "Bārāṇasiṁ pavajjantu,",
  "ja543:7.4_0": "mā ca kañci viheṭhayuṁ”.",
  "ja543:8.1_0": "“Nivesanesu sobbhesu,",
  "ja543:8.2_0": "rathiyā caccaresu ca;",
  "ja543:8.3_0": "Rukkhaggesu ca lambantu,",
  "ja543:8.4_0": "vitatā toraṇesu ca.",
  "ja543:9.1_0": "Ahampi sabbasetena,",
  "ja543:9.2_0": "mahatā sumahaṁ puraṁ;",
  "ja543:9.3_0": "Parikkhipissaṁ bhogehi,",
  "ja543:9.4_0": "kāsīnaṁ janayaṁ bhayaṁ”.",
  "ja543:10.1_0": "Tassa taṁ vacanaṁ sutvā,",
  "ja543:10.2_0": "uragānekavaṇṇino;",
  "ja543:10.3_0": "Bārāṇasiṁ pavajjiṁsu,",
  "ja543:10.4_0": "na ca kañci viheṭhayuṁ.",
  "ja543:11.1_0": "Nivesanesu sobbhesu,",
  "ja543:11.2_0": "rathiyā caccaresu ca;",
  "ja543:11.3_0": "Rukkhaggesu ca lambiṁsu,",
  "ja543:11.4_0": "vitatā toraṇesu ca.",
  "ja543:12.1_0": "Tesu disvāna lambante,",
  "ja543:12.2_0": "puthū kandiṁsu nāriyo;",
  "ja543:12.3_0": "Nāge soṇḍikate disvā,",
  "ja543:12.4_0": "passasante muhuṁ muhuṁ.",
  "ja543:13.1_0": "Bārāṇasī pabyathitā,",
  "ja543:13.2_0": "āturā samapajjatha;",
  "ja543:13.3_0": "Bāhā paggayha pakkanduṁ,",
  "ja543:13.4_0": "“dhītaraṁ dehi rājino”.",
  "ja543:14.1_0": "(…)",
  "ja543:15.1_0": "“Pupphābhihārassa vanassa majjhe,",
  "ja543:15.2_0": "Ko lohitakkho vitatantaraṁso;",
  "ja543:15.3_0": "Kā kambukāyūradharā suvatthā,",
  "ja543:15.4_0": "Tiṭṭhanti nāriyo dasa vandamānā.",
  "ja543:16.1_0": "Ko tvaṁ brahābāhu vanassa majjhe,",
  "ja543:16.2_0": "Virocasi ghatasittova aggi;",
  "ja543:16.3_0": "Mahesakkho aññatarosi yakkho,",
  "ja543:16.4_0": "Udāhu nāgosi mahānubhāvo”.",
  "ja543:17.1_0": "“Nāgohamasmi iddhimā,",
  "ja543:17.2_0": "tejassī duratikkamo;",
  "ja543:17.3_0": "Ḍaṁseyyaṁ tejasā kuddho,",
  "ja543:17.4_0": "phītaṁ janapadaṁ api.",
  "ja543:18.1_0": "Samuddajā hi me mātā,",
  "ja543:18.2_0": "dhataraṭṭho ca me pitā;",
  "ja543:18.3_0": "Sudassanakaniṭṭhosmi,",
  "ja543:18.4_0": "bhūridattoti maṁ vidū.",
  "ja543:19.1_0": "Yaṁ gambhīraṁ sadāvaṭṭaṁ,",
  "ja543:19.2_0": "rahadaṁ bhismaṁ pekkhasi;",
  "ja543:19.3_0": "Esa dibyo mamāvāso,",
  "ja543:19.4_0": "anekasataporiso.",
  "ja543:20.1_0": "Mayūrakoñcābhirudaṁ,",
  "ja543:20.2_0": "nīlodaṁ vanamajjhato;",
  "ja543:20.3_0": "Yamunaṁ pavisa mā bhīto,",
  "ja543:20.4_0": "khemaṁ vattavataṁ sivaṁ.",
  "ja543:21.1_0": "Tattha patto sānucaro,",
  "ja543:21.2_0": "saha puttena brāhmaṇa;",
  "ja543:21.3_0": "Pūjito mayhaṁ kāmehi,",
  "ja543:21.4_0": "sukhaṁ brāhmaṇa vacchasi”.",
  "ja543:22.1_0": "“Samā samantaparito,",
  "ja543:22.2_0": "pahūtatagarā mahī;",
  "ja543:22.3_0": "Indagopakasañchannā,",
  "ja543:22.4_0": "sobhati harituttamā.",
  "ja543:23.1_0": "Rammāni vanacetyāni,",
  "ja543:23.2_0": "rammā haṁsūpakūjitā;",
  "ja543:23.3_0": "Opupphāpadmā tiṭṭhanti,",
  "ja543:23.4_0": "pokkharañño sunimmitā.",
  "ja543:24.1_0": "Aṭṭhaṁsā sukatā thambhā,",
  "ja543:24.2_0": "sabbe veḷuriyāmayā;",
  "ja543:24.3_0": "Sahassathambhā pāsādā,",
  "ja543:24.4_0": "pūrā kaññāhi jotare.",
  "ja543:25.1_0": "Vimānaṁ upapannosi,",
  "ja543:25.2_0": "dibyaṁ puññehi attano;",
  "ja543:25.3_0": "Asambādhaṁ sivaṁ rammaṁ,",
  "ja543:25.4_0": "accantasukhasaṁhitaṁ.",
  "ja543:26.1_0": "Maññe sahassanettassa,",
  "ja543:26.2_0": "vimānaṁ nābhikaṅkhasi;",
  "ja543:26.3_0": "Iddhī hi tyāyaṁ vipulā,",
  "ja543:26.4_0": "sakkasseva jutīmato”.",
  "ja543:27.1_0": "“Manasāpi na pattabbo,",
  "ja543:27.2_0": "ānubhāvo jutīmato;",
  "ja543:27.3_0": "Paricārayamānānaṁ,",
  "ja543:27.4_0": "saindānaṁ vasavattinaṁ.",
  "ja543:28.1_0": "Taṁ vimānaṁ abhijjhāya,",
  "ja543:28.2_0": "amarānaṁ sukhesinaṁ;",
  "ja543:28.3_0": "Uposathaṁ upavasanto,",
  "ja543:28.4_0": "semi vammikamuddhani”.",
  "ja543:29.1_0": "“Ahañca migamesāno,",
  "ja543:29.2_0": "saputto pāvisiṁ vanaṁ;",
  "ja543:29.3_0": "Taṁ maṁ mataṁ vā jīvaṁ vā,",
  "ja543:29.4_0": "nābhivedenti ñātakā.",
  "ja543:30.1_0": "Āmantaye bhūridattaṁ,",
  "ja543:30.2_0": "kāsiputtaṁ yasassinaṁ;",
  "ja543:30.3_0": "Tayā no samanuññātā,",
  "ja543:30.4_0": "api passemu ñātake”.",
  "ja543:31.1_0": "“Eso hi vata me chando,",
  "ja543:31.2_0": "yaṁ vasesi mamantike;",
  "ja543:31.3_0": "Na hi etādisā kāmā,",
  "ja543:31.4_0": "sulabhā honti mānuse.",
  "ja543:32.1_0": "Sace tvaṁ nicchase vatthuṁ,",
  "ja543:32.2_0": "mama kāmehi pūjito;",
  "ja543:32.3_0": "Mayā tvaṁ samanuññāto,",
  "ja543:32.4_0": "sotthiṁ passāhi ñātake”.",
  "ja543:33.1_0": "“Dhārayimaṁ maṇiṁ dibyaṁ,",
  "ja543:33.2_0": "pasuṁ putte ca vindati;",
  "ja543:33.3_0": "Arogo sukhito hoti,",
  "ja543:33.4_0": "gacchevādāya brāhmaṇa”.",
  "ja543:34.1_0": "“Kusalaṁ paṭinandāmi,",
  "ja543:34.2_0": "bhūridatta vaco tava;",
  "ja543:34.3_0": "Pabbajissāmi jiṇṇosmi,",
  "ja543:34.4_0": "na kāme abhipatthaye”.",
  "ja543:35.1_0": "“Brahmacariyassa ce bhaṅgo,",
  "ja543:35.2_0": "hoti bhogehi kāriyaṁ;",
  "ja543:35.3_0": "Avikampamāno eyyāsi,",
  "ja543:35.4_0": "bahuṁ dassāmi te dhanaṁ”.",
  "ja543:36.1_0": "“Kusalaṁ paṭinandāmi,",
  "ja543:36.2_0": "bhūridatta vaco tava;",
  "ja543:36.3_0": "Punapi āgamissāmi,",
  "ja543:36.4_0": "sace attho bhavissati”.",
  "ja543:37.1_0": "Idaṁ vatvā bhūridatto,",
  "ja543:37.2_0": "Pesesi caturo jane;",
  "ja543:37.3_0": "“Etha gacchatha uṭṭhetha,",
  "ja543:37.4_0": "Khippaṁ pāpetha brāhmaṇaṁ”.",
  "ja543:38.1_0": "Tassa taṁ vacanaṁ sutvā,",
  "ja543:38.2_0": "uṭṭhāya caturo janā;",
  "ja543:38.3_0": "Pesitā bhūridattena,",
  "ja543:38.4_0": "khippaṁ pāpesu brāhmaṇaṁ.",
  "ja543:39.1_0": "(…)",
  "ja543:40.1_0": "“Maṇiṁ paggayha maṅgalyaṁ,",
  "ja543:40.2_0": "sādhuvittaṁ manoramaṁ;",
  "ja543:40.3_0": "Selaṁ byañjanasampannaṁ,",
  "ja543:40.4_0": "ko imaṁ maṇimajjhagā”.",
  "ja543:41.1_0": "“Lohitakkhasahassāhi,",
  "ja543:41.2_0": "samantā parivāritaṁ;",
  "ja543:41.3_0": "Ajja kālaṁ pathaṁ gacchaṁ,",
  "ja543:41.4_0": "ajjhagāhaṁ maṇiṁ imaṁ”.",
  "ja543:42.1_0": "“Sūpaciṇṇo ayaṁ selo,",
  "ja543:42.2_0": "accito mānito sadā;",
  "ja543:42.3_0": "Sudhārito sunikkhitto,",
  "ja543:42.4_0": "sabbatthamabhisādhaye.",
  "ja543:43.1_0": "Upacāravipannassa,",
  "ja543:43.2_0": "nikkhepe dhāraṇāya vā;",
  "ja543:43.3_0": "Ayaṁ selo vināsāya,",
  "ja543:43.4_0": "pariciṇṇo ayoniso.",
  "ja543:44.1_0": "Na imaṁ akusalo dibyaṁ,",
  "ja543:44.2_0": "maṇiṁ dhāretumāraho;",
  "ja543:44.3_0": "Paṭipajja sataṁ nikkhaṁ,",
  "ja543:44.4_0": "dehimaṁ ratanaṁ mama”.",
  "ja543:45.1_0": "“Na ca myāyaṁ maṇī keyyo,",
  "ja543:45.2_0": "gohi vā ratanehi vā;",
  "ja543:45.3_0": "Selo byañjanasampanno,",
  "ja543:45.4_0": "neva keyyo maṇī mama”.",
  "ja543:46.1_0": "“No ce tayā maṇī keyyo,",
  "ja543:46.2_0": "gohi vā ratanehi vā;",
  "ja543:46.3_0": "Atha kena maṇī keyyo,",
  "ja543:46.4_0": "taṁ me akkhāhi pucchito”.",
  "ja543:47.1_0": "“Yo me saṁse mahānāgaṁ,",
  "ja543:47.2_0": "tejassiṁ duratikkamaṁ;",
  "ja543:47.3_0": "Tassa dajjaṁ imaṁ selaṁ,",
  "ja543:47.4_0": "jalantamiva tejasā”.",
  "ja543:48.1_0": "“Ko nu brāhmaṇavaṇṇena,",
  "ja543:48.2_0": "supaṇṇo patataṁ varo;",
  "ja543:48.3_0": "Nāgaṁ jigīsamanvesi,",
  "ja543:48.4_0": "anvesaṁ bhakkhamattano”.",
  "ja543:49.1_0": "“Nāhaṁ dijādhipo homi,",
  "ja543:49.2_0": "na diṭṭho garuḷo mayā;",
  "ja543:49.3_0": "Āsīvisena vittoti,",
  "ja543:49.4_0": "vajjo brāhmaṇa maṁ vidū”.",
  "ja543:50.1_0": "“Kiṁ nu tuyhaṁ balaṁ atthi,",
  "ja543:50.2_0": "kiṁ sippaṁ vijjate tava;",
  "ja543:50.3_0": "Kismiṁ vā tvaṁ paratthaddho,",
  "ja543:50.4_0": "uragaṁ nāpacāyasi”.",
  "ja543:51.1_0": "“Āraññikassa isino,",
  "ja543:51.2_0": "cirarattaṁ tapassino;",
  "ja543:51.3_0": "Supaṇṇo kosiyassakkhā,",
  "ja543:51.4_0": "visavijjaṁ anuttaraṁ.",
  "ja543:52.1_0": "Taṁ bhāvitattaññataraṁ,",
  "ja543:52.2_0": "sammantaṁ pabbatantare;",
  "ja543:52.3_0": "Sakkaccaṁ taṁ upaṭṭhāsiṁ,",
  "ja543:52.4_0": "rattindivamatandito.",
  "ja543:53.1_0": "So tadā pariciṇṇo me,",
  "ja543:53.2_0": "vattavā brahmacariyavā;",
  "ja543:53.3_0": "Dibbaṁ pātukarī mantaṁ,",
  "ja543:53.4_0": "kāmasā bhagavā mama.",
  "ja543:54.1_0": "Tyāhaṁ mante paratthaddho,",
  "ja543:54.2_0": "nāhaṁ bhāyāmi bhoginaṁ;",
  "ja543:54.3_0": "Ācariyo visaghātānaṁ,",
  "ja543:54.4_0": "alampānoti maṁ vidū”.",
  "ja543:55.1_0": "“Gaṇhāmase maṇiṁ tāta,",
  "ja543:55.2_0": "somadatta vijānahi;",
  "ja543:55.3_0": "Mā daṇḍena siriṁ pattaṁ,",
  "ja543:55.4_0": "kāmasā pajahimhase”.",
  "ja543:56.1_0": "“Sakaṁ nivesanaṁ pattaṁ,",
  "ja543:56.2_0": "yo taṁ brāhmaṇa pūjayi;",
  "ja543:56.3_0": "Evaṁ kalyāṇakārissa,",
  "ja543:56.4_0": "kiṁ mohā dubbhimicchasi.",
  "ja543:57.1_0": "Sace tvaṁ dhanakāmosi,",
  "ja543:57.2_0": "bhūridatto padassati;",
  "ja543:57.3_0": "Tameva gantvā yācassu,",
  "ja543:57.4_0": "bahuṁ dassati te dhanaṁ”.",
  "ja543:58.1_0": "“Hatthagataṁ pattagataṁ,",
  "ja543:58.2_0": "nikiṇṇaṁ khādituṁ varaṁ;",
  "ja543:58.3_0": "Mā no sandiṭṭhiko attho,",
  "ja543:58.4_0": "somadatta upaccagā”.",
  "ja543:59.1_0": "“Paccati niraye ghore,",
  "ja543:59.2_0": "mahissamapi vivarati;",
  "ja543:59.3_0": "Mittadubbhī hitaccāgī,",
  "ja543:59.4_0": "jīvarevāpi sussati.",
  "ja543:60.1_0": "Sace tvaṁ dhanakāmosi,",
  "ja543:60.2_0": "bhūridatto padassati;",
  "ja543:60.3_0": "Maññe attakataṁ veraṁ,",
  "ja543:60.4_0": "naciraṁ vedayissasi”.",
  "ja543:61.1_0": "“Mahāyaññaṁ yajitvāna,",
  "ja543:61.2_0": "evaṁ sujjhanti brāhmaṇā;",
  "ja543:61.3_0": "Mahāyaññaṁ yajissāma,",
  "ja543:61.4_0": "evaṁ mokkhāma pāpakā”.",
  "ja543:62.1_0": "“Handa dāni apāyāmi,",
  "ja543:62.2_0": "nāhaṁ ajja tayā saha;",
  "ja543:62.3_0": "Padampekaṁ na gaccheyyaṁ,",
  "ja543:62.4_0": "evaṁ kibbisakārinā”.",
  "ja543:63.1_0": "Idaṁ vatvāna pitaraṁ,",
  "ja543:63.2_0": "somadatto bahussuto;",
  "ja543:63.3_0": "Ujjhāpetvāna bhūtāni,",
  "ja543:63.4_0": "tamhā ṭhānā apakkami.",
  "ja543:64.1_0": "“Gaṇhāhetaṁ mahānāgaṁ,",
  "ja543:64.2_0": "āharetaṁ maṇiṁ mama;",
  "ja543:64.3_0": "Indagopakavaṇṇābho,",
  "ja543:64.4_0": "yassa lohitako siro.",
  "ja543:65.1_0": "Kappāsapicurāsīva,",
  "ja543:65.2_0": "eso kāyo padissati;",
  "ja543:65.3_0": "Vammikaggagato seti,",
  "ja543:65.4_0": "taṁ tvaṁ gaṇhāhi brāhmaṇa”.",
  "ja543:66.1_0": "Athosadhehi dibbehi,",
  "ja543:66.2_0": "jappaṁ mantapadāni ca;",
  "ja543:66.3_0": "Evaṁ taṁ asakkhi satthuṁ,",
  "ja543:66.4_0": "katvā parittamattano.",
  "ja543:67.1_0": "(…)",
  "ja543:68.1_0": "“Mamaṁ disvāna āyantaṁ,",
  "ja543:68.2_0": "sabbakāmasamiddhinaṁ;",
  "ja543:68.3_0": "Indriyāni ahaṭṭhāni,",
  "ja543:68.4_0": "sāvaṁ jātaṁ mukhaṁ tava.",
  "ja543:69.1_0": "Padmaṁ yathā hatthagataṁ,",
  "ja543:69.2_0": "Pāṇinā parimadditaṁ;",
  "ja543:69.3_0": "Sāvaṁ jātaṁ mukhaṁ tuyhaṁ,",
  "ja543:69.4_0": "Mamaṁ disvāna edisaṁ.",
  "ja543:70.1_0": "Kacci nu te nābhisasi,",
  "ja543:70.2_0": "kacci te atthi vedanā;",
  "ja543:70.3_0": "Yena sāvaṁ mukhaṁ tuyhaṁ,",
  "ja543:70.4_0": "mamaṁ disvāna āgataṁ”.",
  "ja543:71.1_0": "“Supinaṁ tāta addakkhiṁ,",
  "ja543:71.2_0": "ito māsaṁ adhogataṁ;",
  "ja543:71.3_0": "Dakkhiṇaṁ viya me bāhuṁ,",
  "ja543:71.4_0": "chetvā ruhiramakkhitaṁ;",
  "ja543:71.5_0": "Puriso ādāya pakkāmi,",
  "ja543:71.6_0": "mama rodantiyā sati.",
  "ja543:72.1_0": "Yatohaṁ supinamaddakkhiṁ,",
  "ja543:72.2_0": "sudassana vijānahi;",
  "ja543:72.3_0": "Tato divā vā rattiṁ vā,",
  "ja543:72.4_0": "sukhaṁ me nopalabbhati”.",
  "ja543:73.1_0": "“Yaṁ pubbe parivāriṁsu,",
  "ja543:73.2_0": "kaññā ruciraviggahā;",
  "ja543:73.3_0": "Hemajālapaṭicchannā,",
  "ja543:73.4_0": "bhūridatto na dissati.",
  "ja543:74.1_0": "Yaṁ pubbe parivāriṁsu,",
  "ja543:74.2_0": "nettiṁsavaradhārino;",
  "ja543:74.3_0": "Kaṇikārāva samphullā,",
  "ja543:74.4_0": "bhūridatto na dissati.",
  "ja543:75.1_0": "Handa dāni gamissāma,",
  "ja543:75.2_0": "bhūridattanivesanaṁ;",
  "ja543:75.3_0": "Dhammaṭṭhaṁ sīlasampannaṁ,",
  "ja543:75.4_0": "passāma tava bhātaraṁ”.",
  "ja543:76.1_0": "Tañca disvāna āyantiṁ,",
  "ja543:76.2_0": "bhūridattassa mātaraṁ;",
  "ja543:76.3_0": "Bāhā paggayha pakkanduṁ,",
  "ja543:76.4_0": "bhūridattassa nāriyo.",
  "ja543:77.1_0": "“Puttaṁ teyye na jānāma,",
  "ja543:77.2_0": "ito māsaṁ adhogataṁ;",
  "ja543:77.3_0": "Mataṁ vā yadi vā jīvaṁ,",
  "ja543:77.4_0": "bhūridattaṁ yasassinaṁ.",
  "ja543:78.1_0": "Sakuṇī hataputtāva,",
  "ja543:78.2_0": "suññaṁ disvā kulāvakaṁ;",
  "ja543:78.3_0": "Ciraṁ dukkhena jhāyissaṁ,",
  "ja543:78.4_0": "bhūridattaṁ apassatī.",
  "ja543:79.1_0": "Kurarī hatachāpāva,",
  "ja543:79.2_0": "suññaṁ disvā kulāvakaṁ;",
  "ja543:79.3_0": "Ciraṁ dukkhena jhāyissaṁ,",
  "ja543:79.4_0": "bhūridattaṁ apassatī.",
  "ja543:80.1_0": "Sā nūna cakkavākīva,",
  "ja543:80.2_0": "pallalasmiṁ anodake;",
  "ja543:80.3_0": "Ciraṁ dukkhena jhāyissaṁ,",
  "ja543:80.4_0": "bhūridattaṁ apassatī.",
  "ja543:81.1_0": "Kammārānaṁ yathā ukkā,",
  "ja543:81.2_0": "anto jhāyati no bahi;",
  "ja543:81.3_0": "Evaṁ jhāyāmi sokena,",
  "ja543:81.4_0": "bhūridattaṁ apassatī”.",
  "ja543:82.1_0": "Sālāva sampamathitā,",
  "ja543:82.2_0": "mālutena pamadditā;",
  "ja543:82.3_0": "Senti puttā ca dārā ca,",
  "ja543:82.4_0": "bhūridattanivesane.",
  "ja543:83.1_0": "Idaṁ sutvāna nigghosaṁ,",
  "ja543:83.2_0": "bhūridattanivesane;",
  "ja543:83.3_0": "Ariṭṭho ca subhogo ca,",
  "ja543:83.4_0": "padhāviṁsu anantarā.",
  "ja543:84.1_0": "“Amma assāsa mā soci,",
  "ja543:84.2_0": "evaṁdhammā hi pāṇino;",
  "ja543:84.3_0": "Cavanti upapajjanti,",
  "ja543:84.4_0": "esassa pariṇāmitā”.",
  "ja543:85.1_0": "“Ahampi tāta jānāmi,",
  "ja543:85.2_0": "evaṁdhammā hi pāṇino;",
  "ja543:85.3_0": "Sokena ca paretasmi,",
  "ja543:85.4_0": "bhūridattaṁ apassatī.",
  "ja543:86.1_0": "Ajja ce me imaṁ rattiṁ,",
  "ja543:86.2_0": "sudassana vijānahi;",
  "ja543:86.3_0": "Bhūridattaṁ apassantī,",
  "ja543:86.4_0": "maññe hissāmi jīvitaṁ”.",
  "ja543:87.1_0": "“Amma assāsa mā soci,",
  "ja543:87.2_0": "ānayissāma bhātaraṁ;",
  "ja543:87.3_0": "Disodisaṁ gamissāma,",
  "ja543:87.4_0": "bhātupariyesanaṁ caraṁ.",
  "ja543:88.1_0": "Pabbate giriduggesu,",
  "ja543:88.2_0": "gāmesu nigamesu ca;",
  "ja543:88.3_0": "Orena sattarattassa,",
  "ja543:88.4_0": "bhātaraṁ passa āgataṁ”.",
  "ja543:89.1_0": "“Hatthā pamutto urago,",
  "ja543:89.2_0": "pāde te nipatī bhusaṁ;",
  "ja543:89.3_0": "Kacci nu taṁ ḍaṁsī tāta,",
  "ja543:89.4_0": "mā bhāyi sukhito bhava”.",
  "ja543:90.1_0": "“Neva mayhaṁ ayaṁ nāgo,",
  "ja543:90.2_0": "alaṁ dukkhāya kāyaci;",
  "ja543:90.3_0": "Yāvatatthi ahiggāho,",
  "ja543:90.4_0": "mayā bhiyyo na vijjati”.",
  "ja543:91.1_0": "“Ko nu brāhmaṇavaṇṇena,",
  "ja543:91.2_0": "ditto parisamāgato;",
  "ja543:91.3_0": "Avhāyantu suyuddhena,",
  "ja543:91.4_0": "suṇantu parisā mama”.",
  "ja543:92.1_0": "“Tvaṁ maṁ nāgena ālampa,",
  "ja543:92.2_0": "ahaṁ maṇḍūkachāpiyā;",
  "ja543:92.3_0": "Hotu no abbhutaṁ tattha,",
  "ja543:92.4_0": "ā sahassehi pañcahi”.",
  "ja543:93.1_0": "“Ahañhi vasumā aḍḍho,",
  "ja543:93.2_0": "tvaṁ daliddosi māṇava;",
  "ja543:93.3_0": "Ko nu te pāṭibhogatthi,",
  "ja543:93.4_0": "upajūtañca kiṁ siyā.",
  "ja543:94.1_0": "Upajūtañca me assa,",
  "ja543:94.2_0": "pāṭibhogo ca tādiso;",
  "ja543:94.3_0": "Hotu no abbhutaṁ tattha,",
  "ja543:94.4_0": "ā sahassehi pañcahi”.",
  "ja543:95.1_0": "“Suṇohi me mahārāja,",
  "ja543:95.2_0": "vacanaṁ bhaddamatthu te;",
  "ja543:95.3_0": "Pañcannaṁ me sahassānaṁ,",
  "ja543:95.4_0": "pāṭibhogo hi kittima”.",
  "ja543:96.1_0": "“Pettikaṁ vā iṇaṁ hoti,",
  "ja543:96.2_0": "Yaṁ vā hoti sayaṅkataṁ;",
  "ja543:96.3_0": "Kiṁ tvaṁ evaṁ bahuṁ mayhaṁ,",
  "ja543:96.4_0": "Dhanaṁ yācasi brāhmaṇa”.",
  "ja543:97.1_0": "“Alampāno hi nāgena,",
  "ja543:97.2_0": "mamaṁ abhijigīsati;",
  "ja543:97.3_0": "Ahaṁ maṇḍūkachāpiyā,",
  "ja543:97.4_0": "ḍaṁsayissāmi brāhmaṇaṁ.",
  "ja543:98.1_0": "Taṁ tvaṁ daṭṭhuṁ mahārāja,",
  "ja543:98.2_0": "ajja raṭṭhābhivaḍḍhana;",
  "ja543:98.3_0": "Khattasaṅghaparibyūḷho,",
  "ja543:98.4_0": "niyyāhi ahidassanaṁ”.",
  "ja543:99.1_0": "“Neva taṁ atimaññāmi,",
  "ja543:99.2_0": "sippavādena māṇava;",
  "ja543:99.3_0": "Atimattosi sippena,",
  "ja543:99.4_0": "uragaṁ nāpacāyasi”.",
  "ja543:100.1_0": "“Ahampi nātimaññāmi,",
  "ja543:100.2_0": "sippavādena brāhmaṇa;",
  "ja543:100.3_0": "Avisena ca nāgena,",
  "ja543:100.4_0": "bhusaṁ vañcayase janaṁ.",
  "ja543:101.1_0": "Evañcetaṁ jano jaññā,",
  "ja543:101.2_0": "Yathā jānāmi taṁ ahaṁ;",
  "ja543:101.3_0": "Na tvaṁ labhasi ālampa,",
  "ja543:101.4_0": "Bhusamuṭṭhiṁ kuto dhanaṁ”.",
  "ja543:102.1_0": "“Kharājino jaṭī dummī,",
  "ja543:102.2_0": "ditto parisamāgato;",
  "ja543:102.3_0": "Yo tvaṁ evaṁ gataṁ nāgaṁ,",
  "ja543:102.4_0": "aviso atimaññasi.",
  "ja543:103.1_0": "Āsajja kho naṁ jaññāsi,",
  "ja543:103.2_0": "puṇṇaṁ uggassa tejaso;",
  "ja543:103.3_0": "Maññe taṁ bhasmarāsiṁva,",
  "ja543:103.4_0": "khippameso karissati”.",
  "ja543:104.1_0": "“Siyā visaṁ siluttassa,",
  "ja543:104.2_0": "deḍḍubhassa silābhuno;",
  "ja543:104.3_0": "Neva lohitasīsassa,",
  "ja543:104.4_0": "visaṁ nāgassa vijjati”.",
  "ja543:105.1_0": "“Sutametaṁ arahataṁ,",
  "ja543:105.2_0": "saññatānaṁ tapassinaṁ;",
  "ja543:105.3_0": "Idha dānāni datvāna,",
  "ja543:105.4_0": "saggaṁ gacchanti dāyakā;",
  "ja543:105.5_0": "Jīvanto dehi dānāni,",
  "ja543:105.6_0": "yadi te atthi dātave.",
  "ja543:106.1_0": "Ayaṁ nāgo mahiddhiko,",
  "ja543:106.2_0": "tejassī duratikkamo;",
  "ja543:106.3_0": "Tena taṁ ḍaṁsayissāmi,",
  "ja543:106.4_0": "so taṁ bhasmaṁ karissati”.",
  "ja543:107.1_0": "“Mayāpetaṁ sutaṁ samma,",
  "ja543:107.2_0": "saññatānaṁ tapassinaṁ;",
  "ja543:107.3_0": "Idha dānāni datvāna,",
  "ja543:107.4_0": "saggaṁ gacchanti dāyakā;",
  "ja543:107.5_0": "Tvameva dehi jīvanto,",
  "ja543:107.6_0": "yadi te atthi dātave.",
  "ja543:108.1_0": "Ayaṁ ajamukhī nāma,",
  "ja543:108.2_0": "puṇṇā uggassa tejaso;",
  "ja543:108.3_0": "Tāya taṁ ḍaṁsayissāmi,",
  "ja543:108.4_0": "sā taṁ bhasmaṁ karissati.",
  "ja543:109.1_0": "Yā dhītā dhataraṭṭhassa,",
  "ja543:109.2_0": "vemātā bhaginī mama;",
  "ja543:109.3_0": "Sā taṁ ḍaṁsatvajamukhī,",
  "ja543:109.4_0": "puṇṇā uggassa tejaso.",
  "ja543:110.1_0": "Chamāyañce nisiñcissaṁ,",
  "ja543:110.2_0": "brahmadatta vijānahi;",
  "ja543:110.3_0": "Tiṇalatāni osadhyo,",
  "ja543:110.4_0": "ussusseyyuṁ asaṁsayaṁ.",
  "ja543:111.1_0": "Uddhañce pātayissāmi,",
  "ja543:111.2_0": "brahmadatta vijānahi;",
  "ja543:111.3_0": "Satta vassāniyaṁ devo,",
  "ja543:111.4_0": "na vasse na himaṁ pate.",
  "ja543:112.1_0": "Udake ce nisiñcissaṁ,",
  "ja543:112.2_0": "brahmadatta vijānahi;",
  "ja543:112.3_0": "Yāvantodakajā pāṇā,",
  "ja543:112.4_0": "mareyyuṁ macchakacchapā”.",
  "ja543:113.1_0": "(…)",
  "ja543:114.1_0": "“Lokyaṁ sajantaṁ udakaṁ,",
  "ja543:114.2_0": "payāgasmiṁ patiṭṭhitaṁ;",
  "ja543:114.3_0": "Komaṁ ajjhoharī bhūto,",
  "ja543:114.4_0": "ogāḷhaṁ yamunaṁ nadiṁ”.",
  "ja543:115.1_0": "“Yadesa lokādhipatī yasassī,",
  "ja543:115.2_0": "Bārāṇasiṁ pakriya samantato;",
  "ja543:115.3_0": "Tassāha putto uragūsabhassa,",
  "ja543:115.4_0": "Subhogoti maṁ brāhmaṇa vedayanti”.",
  "ja543:116.1_0": "“Sace hi putto uragūsabhassa,",
  "ja543:116.2_0": "Kāsissa rañño amarādhipassa;",
  "ja543:116.3_0": "Mahesakkho aññataro pitā te,",
  "ja543:116.4_0": "Maccesu mātā pana te atulyā;",
  "ja543:116.5_0": "Na tādiso arahati brāhmaṇassa,",
  "ja543:116.6_0": "Dāsampi ohārituṁ mahānubhāvo”.",
  "ja543:117.1_0": "“Rukkhaṁ nissāya vijjhittho,",
  "ja543:117.2_0": "eṇeyyaṁ pātumāgataṁ;",
  "ja543:117.3_0": "So viddho dūramacari,",
  "ja543:117.4_0": "saravegena sīghavā.",
  "ja543:118.1_0": "Taṁ tvaṁ patitamaddakkhi,",
  "ja543:118.2_0": "araññasmiṁ brahāvane;",
  "ja543:118.3_0": "Sa maṁsakājamādāya,",
  "ja543:118.4_0": "sāyaṁ nigrodhupāgami.",
  "ja543:119.1_0": "Sukasāḷikasaṅghuṭṭhaṁ,",
  "ja543:119.2_0": "piṅgalaṁ santhatāyutaṁ;",
  "ja543:119.3_0": "Kokilābhirudaṁ rammaṁ,",
  "ja543:119.4_0": "dhuvaṁ haritasaddalaṁ.",
  "ja543:120.1_0": "Tattha te so pāturahu,",
  "ja543:120.2_0": "iddhiyā yasasā jalaṁ;",
  "ja543:120.3_0": "Mahānubhāvo bhātā me,",
  "ja543:120.4_0": "kaññāhi parivārito.",
  "ja543:121.1_0": "So tena pariciṇṇo tvaṁ,",
  "ja543:121.2_0": "sabbakāmehi tappito;",
  "ja543:121.3_0": "Aduṭṭhassa tuvaṁ dubbhi,",
  "ja543:121.4_0": "taṁ te veraṁ idhāgataṁ.",
  "ja543:122.1_0": "Khippaṁ gīvaṁ pasārehi,",
  "ja543:122.2_0": "na te dassāmi jīvitaṁ;",
  "ja543:122.3_0": "Bhātu parisaraṁ veraṁ,",
  "ja543:122.4_0": "chedayissāmi te siraṁ”.",
  "ja543:123.1_0": "“Ajjhāyako yācayogī,",
  "ja543:123.2_0": "āhutaggi ca brāhmaṇo;",
  "ja543:123.3_0": "Etehi tīhi ṭhānehi,",
  "ja543:123.4_0": "avajjho hoti brāhmaṇo”.",
  "ja543:124.1_0": "“Yaṁ pūraṁ dhataraṭṭhassa,",
  "ja543:124.2_0": "ogāḷhaṁ yamunaṁ nadiṁ;",
  "ja543:124.3_0": "Jotate sabbasovaṇṇaṁ,",
  "ja543:124.4_0": "girimāhacca yāmunaṁ.",
  "ja543:125.1_0": "Tattha te purisabyagghā,",
  "ja543:125.2_0": "sodariyā mama bhātaro;",
  "ja543:125.3_0": "Yathā te tattha vakkhanti,",
  "ja543:125.4_0": "tathā hessasi brāhmaṇa”.",
  "ja543:126.1_0": "(…)",
  "ja543:127.1_0": "“Anittarā ittarasampayuttā,",
  "ja543:127.2_0": "Yaññā ca vedā ca subhoga loke;",
  "ja543:127.3_0": "Tadaggarayhañhi vinindamāno,",
  "ja543:127.4_0": "Jahāti vittañca satañca dhammaṁ.",
  "ja543:128.1_0": "Ajjhenamariyā pathaviṁ janindā,",
  "ja543:128.2_0": "Vessā kasiṁ pāricariyañca suddā;",
  "ja543:128.3_0": "Upāgu paccekaṁ yathāpadesaṁ,",
  "ja543:128.4_0": "Katāhu ete vasināti āhu.",
  "ja543:129.1_0": "Dhātā vidhātā varuṇo kuvero,",
  "ja543:129.2_0": "Somo yamo candimā vāyu sūriyo;",
  "ja543:129.3_0": "Etepi yaññaṁ puthuso yajitvā,",
  "ja543:129.4_0": "Ajjhāyakānaṁ atho sabbakāme.",
  "ja543:130.1_0": "Vikāsitā cāpasatāni pañca,",
  "ja543:130.2_0": "Yo ajjuno balavā bhīmaseno;",
  "ja543:130.3_0": "Sahassabāhu asamo pathabyā,",
  "ja543:130.4_0": "Sopi tadā mādahi jātavedaṁ.",
  "ja543:131.1_0": "Yo brāhmaṇe bhojayi dīgharattaṁ,",
  "ja543:131.2_0": "Annena pānena yathānubhāvaṁ;",
  "ja543:131.3_0": "Pasannacitto anumodamāno,",
  "ja543:131.4_0": "Subhoga devaññataro ahosi.",
  "ja543:132.1_0": "Mahāsanaṁ devamanomavaṇṇaṁ,",
  "ja543:132.2_0": "Yo sappinā asakkhi bhojetumaggiṁ;",
  "ja543:132.3_0": "Sa yaññatantaṁ varato yajitvā,",
  "ja543:132.4_0": "Dibbaṁ gatiṁ mucalindajjhagacchi.",
  "ja543:133.1_0": "Mahānubhāvo vassasahassajīvī,",
  "ja543:133.2_0": "Yo pabbajī dassaneyyo uḷāro;",
  "ja543:133.3_0": "Hitvā apariyanta raṭṭhaṁ sasenaṁ,",
  "ja543:133.4_0": "Rājā dudīpopi jagāma saggaṁ.",
  "ja543:134.1_0": "Yo sāgarantaṁ sāgaro vijitvā,",
  "ja543:134.2_0": "Yūpaṁ subhaṁ soṇṇamayaṁ uḷāraṁ;",
  "ja543:134.3_0": "Ussesi vessānaramādahāno,",
  "ja543:134.4_0": "Subhoga devaññataro ahosi.",
  "ja543:135.1_0": "Yassānubhāvena subhoga gaṅgā,",
  "ja543:135.2_0": "Pavattatha dadhisannisinnaṁ samuddaṁ;",
  "ja543:135.3_0": "Salomapādo paricariya maggiṁ,",
  "ja543:135.4_0": "Aṅgo sahassakkhapurajjhagacchi.",
  "ja543:136.1_0": "Mahiddhiko devavaro yasassī,",
  "ja543:136.2_0": "Senāpati tidive vāsavassa;",
  "ja543:136.3_0": "So somayāgena malaṁ vihantvā,",
  "ja543:136.4_0": "Subhoga devaññataro ahosi.",
  "ja543:137.1_0": "Akārayi lokamimaṁ parañca,",
  "ja543:137.2_0": "Bhāgīrathiṁ himavantañca gijjhaṁ;",
  "ja543:137.3_0": "Yo iddhimā devavaro yasassī,",
  "ja543:137.4_0": "Sopi tadā ādahi jātavedaṁ.",
  "ja543:138.1_0": "Mālāgirī himavā yo ca gijjho,",
  "ja543:138.2_0": "Sudassano nisabho kuveru;",
  "ja543:138.3_0": "Ete ca aññe ca nagā mahantā,",
  "ja543:138.4_0": "Cityā katā yaññakarehi māhu.",
  "ja543:139.1_0": "Ajjhāyakaṁ mantaguṇūpapannaṁ,",
  "ja543:139.2_0": "Tapassinaṁ yācayogotidhāhu;",
  "ja543:139.3_0": "Tīre samuddassudakaṁ sajantaṁ,",
  "ja543:139.4_0": "Taṁ sāgarojjhohari tenapeyyo.",
  "ja543:140.1_0": "Āyāgavatthūni puthū pathabyā,",
  "ja543:140.2_0": "Saṁvijjanti brāhmaṇā vāsavassa;",
  "ja543:140.3_0": "Purimaṁ disaṁ pacchimaṁ dakkhiṇuttaraṁ,",
  "ja543:140.4_0": "Saṁvijjamānā janayanti vedaṁ”.",
  "ja543:141.1_0": "“Kalī hi dhīrāna kaṭaṁ magānaṁ,",
  "ja543:141.2_0": "Bhavanti vedajjhagatānariṭṭha;",
  "ja543:141.3_0": "Marīcidhammaṁ asamekkhitattā,",
  "ja543:141.4_0": "Māyāguṇā nātivahanti paññaṁ.",
  "ja543:142.1_0": "Vedā na tāṇāya bhavanti dassa,",
  "ja543:142.2_0": "Mittadduno bhūnahuno narassa;",
  "ja543:142.3_0": "Na tāyate pariciṇṇo ca aggi,",
  "ja543:142.4_0": "Dosantaraṁ maccamanariyakammaṁ.",
  "ja543:143.1_0": "Sabbañca maccā sadhanaṁ sabhogaṁ,",
  "ja543:143.2_0": "Ādīpitaṁ dāru tiṇena missaṁ;",
  "ja543:143.3_0": "Dahaṁ na tappe asamatthatejo,",
  "ja543:143.4_0": "Ko taṁ subhikkhaṁ dvirasaññu kayirā.",
  "ja543:144.1_0": "Yathāpi khīraṁ vipariṇāmadhammaṁ,",
  "ja543:144.2_0": "Dadhi bhavitvā navanītampi hoti;",
  "ja543:144.3_0": "Evampi aggi vipariṇāmadhammo,",
  "ja543:144.4_0": "Tejo samorohatī yogayutto.",
  "ja543:145.1_0": "Na dissatī aggimanuppaviṭṭho,",
  "ja543:145.2_0": "Sukkhesu kaṭṭhesu navesu cāpi;",
  "ja543:145.3_0": "Nāmatthamāno araṇīnarena,",
  "ja543:145.4_0": "Nākammunā jāyati jātavedo.",
  "ja543:146.1_0": "Sace hi aggi antarato vaseyya,",
  "ja543:146.2_0": "Sukkhesu kaṭṭhesu navesu cāpi;",
  "ja543:146.3_0": "Sabbāni susseyyu vanāni loke,",
  "ja543:146.4_0": "Sukkhāni kaṭṭhāni ca pajjaleyyuṁ.",
  "ja543:147.1_0": "Karoti ce dārutiṇena puññaṁ,",
  "ja543:147.2_0": "Bhojaṁ naro dhūmasikhiṁ patāpavaṁ;",
  "ja543:147.3_0": "Aṅgārikā loṇakarā ca sūdā,",
  "ja543:147.4_0": "Sarīradāhāpi kareyyu puññaṁ.",
  "ja543:148.1_0": "Atha ce hi ete na karonti puññaṁ,",
  "ja543:148.2_0": "Ajjhenamaggiṁ idha tappayitvā;",
  "ja543:148.3_0": "Na koci lokasmiṁ karoti puññaṁ,",
  "ja543:148.4_0": "Bhojaṁ naro dhūmasikhiṁ patāpavaṁ.",
  "ja543:149.1_0": "Kathañhi lokāpacito samāno,",
  "ja543:149.2_0": "Amanuññagandhaṁ bahūnaṁ akantaṁ;",
  "ja543:149.3_0": "Yadeva maccā parivajjayanti,",
  "ja543:149.4_0": "Tadappasatthaṁ dvirasaññu bhuñje.",
  "ja543:150.1_0": "Sikhimpi devesu vadanti heke,",
  "ja543:150.2_0": "Āpaṁ milakkhū pana devamāhu;",
  "ja543:150.3_0": "Sabbeva ete vitathaṁ bhaṇanti,",
  "ja543:150.4_0": "Aggī na devaññataro na cāpo.",
  "ja543:151.1_0": "Anindriyabaddhamasaññakāyaṁ,",
  "ja543:151.2_0": "Vessānaraṁ kammakaraṁ pajānaṁ;",
  "ja543:151.3_0": "Paricariya maggiṁ sugatiṁ kathaṁ vaje,",
  "ja543:151.4_0": "Pāpāni kammāni pakubbamāno.",
  "ja543:152.1_0": "Sabbābhibhū tāhudha jīvikatthā,",
  "ja543:152.2_0": "Aggissa brahmā paricārakoti;",
  "ja543:152.3_0": "Sabbānubhāvī ca vasī kimatthaṁ,",
  "ja543:152.4_0": "Animmito nimmitaṁ vanditassa.",
  "ja543:153.1_0": "Hassaṁ anijjhānakhamaṁ atacchaṁ,",
  "ja543:153.2_0": "Sakkārahetu pakiriṁsu pubbe;",
  "ja543:153.3_0": "Te lābhasakkāre apātubhonte,",
  "ja543:153.4_0": "Sandhāpitā jantubhi santidhammaṁ.",
  "ja543:154.1_0": "Ajjhenamariyā pathaviṁ janindā,",
  "ja543:154.2_0": "Vessā kasiṁ pāricariyañca suddā;",
  "ja543:154.3_0": "Upāgu paccekaṁ yathāpadesaṁ,",
  "ja543:154.4_0": "Katāhu ete vasināti āhu.",
  "ja543:155.1_0": "Etañca saccaṁ vacanaṁ bhaveyya,",
  "ja543:155.2_0": "Yathā idaṁ bhāsitaṁ brāhmaṇehi;",
  "ja543:155.3_0": "Nākhattiyo jātu labhetha rajjaṁ,",
  "ja543:155.4_0": "Nābrāhmaṇo mantapadāni sikkhe;",
  "ja543:155.5_0": "Nāññatra vessehi kasiṁ kareyya,",
  "ja543:155.6_0": "Suddo na mucce parapesanāya.",
  "ja543:156.1_0": "Yasmā ca etaṁ vacanaṁ abhūtaṁ,",
  "ja543:156.2_0": "Musāvime odariyā bhaṇanti;",
  "ja543:156.3_0": "Tadappapaññā abhisaddahanti,",
  "ja543:156.4_0": "Passanti taṁ paṇḍitā attanāva.",
  "ja543:157.1_0": "Khatyā hi vessānaṁ baliṁ haranti,",
  "ja543:157.2_0": "Ādāya satthāni caranti brāhmaṇā;",
  "ja543:157.3_0": "Taṁ tādisaṁ saṅkhubhitaṁ pabhinnaṁ,",
  "ja543:157.4_0": "Kasmā brahmā nujju karoti lokaṁ.",
  "ja543:158.1_0": "Sace hi so issaro sabbaloke,",
  "ja543:158.2_0": "Brahmā bahūbhūtapatī pajānaṁ;",
  "ja543:158.3_0": "Kiṁ sabbalokaṁ vidahī alakkhiṁ,",
  "ja543:158.4_0": "Kiṁ sabbalokaṁ na sukhiṁ akāsi.",
  "ja543:159.1_0": "Sace hi so issaro sabbaloke,",
  "ja543:159.2_0": "Brahmā bahūbhūtapatī pajānaṁ;",
  "ja543:159.3_0": "Māyā musāvajjamadena cāpi,",
  "ja543:159.4_0": "Lokaṁ adhammena kimatthamakāri.",
  "ja543:160.1_0": "Sace hi so issaro sabbaloke,",
  "ja543:160.2_0": "Brahmā bahūbhūtapatī pajānaṁ;",
  "ja543:160.3_0": "Adhammiko bhūtapatī ariṭṭha,",
  "ja543:160.4_0": "Dhamme sati yo vidahī adhammaṁ.",
  "ja543:161.1_0": "Kīṭā paṭaṅgā uragā ca bhekā,",
  "ja543:161.2_0": "Hantvā kimī sujjhati makkhikā ca;",
  "ja543:161.3_0": "Etepi dhammā anariyarūpā,",
  "ja543:161.4_0": "Kambojakānaṁ vitathā bahūnaṁ.",
  "ja543:162.1_0": "Sace hi so sujjhati yo hanāti,",
  "ja543:162.2_0": "Hatopi so saggamupeti ṭhānaṁ;",
  "ja543:162.3_0": "Bhovādi bhovādina mārayeyyuṁ,",
  "ja543:162.4_0": "Ye cāpi tesaṁ abhisaddaheyyuṁ.",
  "ja543:163.1_0": "Neva migā na pasū nopi gāvo,",
  "ja543:163.2_0": "Āyācanti attavadhāya keci;",
  "ja543:163.3_0": "Vipphandamāne idha jīvikatthā,",
  "ja543:163.4_0": "Yaññesu pāṇe pasumārabhanti.",
  "ja543:164.1_0": "Yūpussane pasubandhe ca bālā,",
  "ja543:164.2_0": "Cittehi vaṇṇehi mukhaṁ nayanti;",
  "ja543:164.3_0": "Ayaṁ te yūpo kāmaduho parattha,",
  "ja543:164.4_0": "Bhavissati sassato samparāye.",
  "ja543:165.1_0": "Sace ca yūpe maṇisaṅkhamuttaṁ,",
  "ja543:165.2_0": "Dhaññaṁ dhanaṁ rajataṁ jātarūpaṁ;",
  "ja543:165.3_0": "Sukkhesu kaṭṭhesu navesu cāpi,",
  "ja543:165.4_0": "Sace duhe tidive sabbakāme;",
  "ja543:165.5_0": "Tevijjasaṅghāva puthū yajeyyuṁ,",
  "ja543:165.6_0": "Abrāhmaṇaṁ kañci na yājayeyyuṁ.",
  "ja543:166.1_0": "Kuto ca yūpe maṇisaṅkhamuttaṁ,",
  "ja543:166.2_0": "Dhaññaṁ dhanaṁ rajataṁ jātarūpaṁ;",
  "ja543:166.3_0": "Sukkhesu kaṭṭhesu navesu cāpi,",
  "ja543:166.4_0": "Kuto duhe tidive sabbakāme.",
  "ja543:167.1_0": "Saṭhā ca luddā ca paluddhabālā,",
  "ja543:167.2_0": "Cittehi vaṇṇehi mukhaṁ nayanti;",
  "ja543:167.3_0": "Ādāya aggiṁ mama dehi vittaṁ,",
  "ja543:167.4_0": "Tato sukhī hohisi sabbakāme.",
  "ja543:168.1_0": "Tamaggihuttaṁ saraṇaṁ pavissa,",
  "ja543:168.2_0": "Cittehi vaṇṇehi mukhaṁ nayanti;",
  "ja543:168.3_0": "Oropayitvā kesamassuṁ nakhañca,",
  "ja543:168.4_0": "Vedehi vittaṁ atigāḷhayanti.",
  "ja543:169.1_0": "Kākā ulūkaṁva raho labhitvā,",
  "ja543:169.2_0": "Ekaṁ samānaṁ bahukā samecca;",
  "ja543:169.3_0": "Annāni bhutvā kuhakā kuhitvā,",
  "ja543:169.4_0": "Muṇḍaṁ karitvā yaññapathossajanti.",
  "ja543:170.1_0": "Evañhi so vañcito brāhmaṇehi,",
  "ja543:170.2_0": "Eko samāno bahukā samecca;",
  "ja543:170.3_0": "Te yogayogena vilumpamānā,",
  "ja543:170.4_0": "Diṭṭhaṁ adiṭṭhena dhanaṁ haranti.",
  "ja543:171.1_0": "Akāsiyā rājūhivānusiṭṭhā,",
  "ja543:171.2_0": "Tadassa ādāya dhanaṁ haranti;",
  "ja543:171.3_0": "Te tādisā corasamā asantā,",
  "ja543:171.4_0": "Vajjhā na haññanti ariṭṭha loke.",
  "ja543:172.1_0": "Indassa bāhārasi dakkhiṇāti,",
  "ja543:172.2_0": "Yaññesu chindanti palāsayaṭṭhiṁ;",
  "ja543:172.3_0": "Tañcepi saccaṁ maghavā chinnabāhu,",
  "ja543:172.4_0": "Kenassa indo asure jināti.",
  "ja543:173.1_0": "Tañceva tucchaṁ maghavā samaṅgī,",
  "ja543:173.2_0": "Hantā avajjho paramo sa devo;",
  "ja543:173.3_0": "Mantā ime brāhmaṇā tuccharūpā,",
  "ja543:173.4_0": "Sandiṭṭhikā vañcanā esa loke.",
  "ja543:174.1_0": "Mālāgiri himavā yo ca gijjho,",
  "ja543:174.2_0": "Sudassano nisabho kuveru;",
  "ja543:174.3_0": "Ete ca aññe ca nagā mahantā,",
  "ja543:174.4_0": "Cityā katā yaññakarehi māhu.",
  "ja543:175.1_0": "Yathāpakārāni hi iṭṭhakāni,",
  "ja543:175.2_0": "Cityā katā yaññakarehi māhu;",
  "ja543:175.3_0": "Na pabbatā honti tathāpakārā,",
  "ja543:175.4_0": "Aññā disā acalā tiṭṭhaselā.",
  "ja543:176.1_0": "Na iṭṭhakā honti silā cirena,",
  "ja543:176.2_0": "Na tattha sañjāyati ayo na lohaṁ;",
  "ja543:176.3_0": "Yaññañca etaṁ parivaṇṇayantā,",
  "ja543:176.4_0": "Cityā katā yaññakarehi māhu.",
  "ja543:177.1_0": "Ajjhāyakaṁ mantaguṇūpapannaṁ,",
  "ja543:177.2_0": "Tapassinaṁ yācayogotidhāhu;",
  "ja543:177.3_0": "Tīre samuddassudakaṁ sajantaṁ,",
  "ja543:177.4_0": "Taṁ sāgarojjhohari tenapeyyo.",
  "ja543:178.1_0": "Parosahassampi samantavede,",
  "ja543:178.2_0": "Mantūpapanne nadiyo vahanti;",
  "ja543:178.3_0": "Na tena byāpannarasūdakā na,",
  "ja543:178.4_0": "Kasmā samuddo atulo apeyyo.",
  "ja543:179.1_0": "Ye keci kūpā idha jīvaloke,",
  "ja543:179.2_0": "Loṇūdakā kūpakhaṇehi khātā;",
  "ja543:179.3_0": "Na brāhmaṇajjhoharaṇena tesu,",
  "ja543:179.4_0": "Āpo apeyyo dvirasaññu māhu.",
  "ja543:180.1_0": "Pure puratthā kā kassa bhariyā,",
  "ja543:180.2_0": "Mano manussaṁ ajanesi pubbe;",
  "ja543:180.3_0": "Tenāpi dhammena na koci hīno,",
  "ja543:180.4_0": "Evampi vosaggavibhaṅgamāhu.",
  "ja543:181.1_0": "Caṇḍālaputtopi adhicca vede,",
  "ja543:181.2_0": "Bhāseyya mante kusalo matīmā;",
  "ja543:181.3_0": "Na tassa muddhāpi phaleyya sattadhā,",
  "ja543:181.4_0": "Mantā ime attavadhāya katā.",
  "ja543:182.1_0": "Vācākatā giddhikatā gahītā,",
  "ja543:182.2_0": "Dummocayā kabyapathānupannā;",
  "ja543:182.3_0": "Bālāna cittaṁ visame niviṭṭhaṁ,",
  "ja543:182.4_0": "Tadappapaññā abhisaddahanti.",
  "ja543:183.1_0": "Sīhassa byagghassa ca dīpino ca,",
  "ja543:183.2_0": "Na vijjatī porisiyaṁ balena;",
  "ja543:183.3_0": "Manussabhāvo ca gavaṁva pekkho,",
  "ja543:183.4_0": "Jātī hi tesaṁ asamā samānā.",
  "ja543:184.1_0": "Sace ca rājā pathaviṁ vijitvā,",
  "ja543:184.2_0": "Sajīvavā assavapārisajjo;",
  "ja543:184.3_0": "Sayameva so sattusaṅghaṁ vijeyya,",
  "ja543:184.4_0": "Tassappajā niccasukhī bhaveyya.",
  "ja543:185.1_0": "Khattiyamantā ca tayo ca vedā,",
  "ja543:185.2_0": "Atthena ete samakā bhavanti;",
  "ja543:185.3_0": "Tesañca atthaṁ avinicchinitvā,",
  "ja543:185.4_0": "Na bujjhatī oghapathaṁva channaṁ.",
  "ja543:186.1_0": "Khattiyamantā ca tayo ca vedā,",
  "ja543:186.2_0": "Atthena ete samakā bhavanti;",
  "ja543:186.3_0": "Lābho alābho ayaso yaso ca,",
  "ja543:186.4_0": "Sabbeva tesaṁ catunnañca dhammā.",
  "ja543:187.1_0": "Yathāpi ibbhā dhanadhaññahetu,",
  "ja543:187.2_0": "Kammāni karonti puthū pathabyā;",
  "ja543:187.3_0": "Tevijjasaṅghā ca tatheva ajja,",
  "ja543:187.4_0": "Kammāni karonti puthū pathabyā.",
  "ja543:188.1_0": "Ibbhehi ye te samakā bhavanti,",
  "ja543:188.2_0": "Niccussukā kāmaguṇesu yuttā;",
  "ja543:188.3_0": "Kammāni karonti puthū pathabyā,",
  "ja543:188.4_0": "Tadappapaññā dvirasaññurā te”.",
  "ja543:189.1_0": "(…)",
  "ja543:190.1_0": "“Kassa bherī mudiṅgā ca,",
  "ja543:190.2_0": "saṅkhāpaṇavadindimā;",
  "ja543:190.3_0": "Purato paṭipannāni,",
  "ja543:190.4_0": "hāsayantā rathesabhaṁ.",
  "ja543:191.1_0": "Kassa kañcanapaṭṭena,",
  "ja543:191.2_0": "puthunā vijjuvaṇṇinā;",
  "ja543:191.3_0": "Yuvā kalāpasannaddho,",
  "ja543:191.4_0": "ko eti siriyā jalaṁ.",
  "ja543:192.1_0": "Ukkāmukhapahaṭṭhaṁva,",
  "ja543:192.2_0": "khadiraṅgārasannibhaṁ;",
  "ja543:192.3_0": "Mukhañca rucirā bhāti,",
  "ja543:192.4_0": "ko eti siriyā jalaṁ.",
  "ja543:193.1_0": "Kassa jambonadaṁ chattaṁ,",
  "ja543:193.2_0": "sasalākaṁ manoramaṁ;",
  "ja543:193.3_0": "Ādiccaraṁsāvaraṇaṁ,",
  "ja543:193.4_0": "ko eti siriyā jalaṁ.",
  "ja543:194.1_0": "Kassa aṅgaṁ pariggayha,",
  "ja543:194.2_0": "vālabījanimuttamaṁ;",
  "ja543:194.3_0": "Ubhato varapuññassa,",
  "ja543:194.4_0": "muddhani uparūpari.",
  "ja543:195.1_0": "Kassa pekhuṇahatthāni,",
  "ja543:195.2_0": "citrāni ca mudūni ca;",
  "ja543:195.3_0": "Kañcanamaṇidaṇḍāni,",
  "ja543:195.4_0": "caranti dubhato mukhaṁ.",
  "ja543:196.1_0": "Khadiraṅgāravaṇṇābhā,",
  "ja543:196.2_0": "ukkāmukhapahaṁsitā;",
  "ja543:196.3_0": "Kassete kuṇḍalā vaggū,",
  "ja543:196.4_0": "sobhanti dubhato mukhaṁ.",
  "ja543:197.1_0": "Kassa vātena chupitā,",
  "ja543:197.2_0": "niddhantā mudukāḷakā;",
  "ja543:197.3_0": "Sobhayanti nalāṭantaṁ,",
  "ja543:197.4_0": "nabhā vijjurivuggatā.",
  "ja543:198.1_0": "Kassa etāni akkhīni,",
  "ja543:198.2_0": "āyatāni puthūni ca;",
  "ja543:198.3_0": "Ko sobhati visālakkho,",
  "ja543:198.4_0": "kassetaṁ uṇṇajaṁ mukhaṁ.",
  "ja543:199.1_0": "Kassete lapanajātā,",
  "ja543:199.2_0": "suddhā saṅkhavarūpamā;",
  "ja543:199.3_0": "Bhāsamānassa sobhanti,",
  "ja543:199.4_0": "dantā kuppilasādisā.",
  "ja543:200.1_0": "Kassa lākhārasasamā,",
  "ja543:200.2_0": "hatthapādā sukhedhitā;",
  "ja543:200.3_0": "Ko so bimboṭṭhasampanno,",
  "ja543:200.4_0": "divā sūriyova bhāsati.",
  "ja543:201.1_0": "Himaccaye himavati,",
  "ja543:201.2_0": "mahāsālova pupphito;",
  "ja543:201.3_0": "Ko so odātapāvāro,",
  "ja543:201.4_0": "jayaṁ indova sobhati.",
  "ja543:202.1_0": "Suvaṇṇapīḷakākiṇṇaṁ,",
  "ja543:202.2_0": "maṇidaṇḍavicittakaṁ;",
  "ja543:202.3_0": "Ko so parisamogayha,",
  "ja543:202.4_0": "īsaṁ khaggaṁ pamuñcati.",
  "ja543:203.1_0": "Suvaṇṇavikatā cittā,",
  "ja543:203.2_0": "sukatā cittasibbanā;",
  "ja543:203.3_0": "Ko so omuñcate pādā,",
  "ja543:203.4_0": "namo katvā mahesino”.",
  "ja543:204.1_0": "“Dhataraṭṭhā hi te nāgā,",
  "ja543:204.2_0": "iddhimanto yasassino;",
  "ja543:204.3_0": "Samuddajāya uppannā,",
  "ja543:204.4_0": "nāgā ete mahiddhikā”ti.",
  "ja543:205.1_0": "Bhūridattajātakaṁ chaṭṭhaṁ."
}