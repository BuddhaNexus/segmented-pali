{
  "ja512:0.1_0": "Jātaka",
  "ja512:0.2_0": "Tiṁsanipāta",
  "ja512:0.3_0": "Kiṁchandavagga",
  "ja512:0.4_0": "2. Kumbhajātaka",
  "ja512:1.1_0": "“Ko pāturāsī tidivā nabhamhi,",
  "ja512:1.2_0": "Obhāsayaṁ saṁvariṁ candimāva;",
  "ja512:1.3_0": "Gattehi te rasmiyo niccharanti,",
  "ja512:1.4_0": "Sateratā vijjurivantalikkhe.",
  "ja512:2.1_0": "So chinnavātaṁ kamasī aghamhi,",
  "ja512:2.2_0": "Vehāyasaṁ gacchasi tiṭṭhasī ca;",
  "ja512:2.3_0": "Iddhī nu te vatthukatā subhāvitā,",
  "ja512:2.4_0": "Anaddhagūnaṁ api devatānaṁ.",
  "ja512:3.1_0": "Vehāyasaṁ gammamāgamma tiṭṭhasi,",
  "ja512:3.2_0": "Kumbhaṁ kiṇāthāti yametamatthaṁ;",
  "ja512:3.3_0": "Ko vā tuvaṁ kissa vā tāya kumbho,",
  "ja512:3.4_0": "Akkhāhi me brāhmaṇa etamatthaṁ”.",
  "ja512:4.1_0": "“Na sappikumbho napi telakumbho,",
  "ja512:4.2_0": "Na phāṇitassa na madhussa kumbho;",
  "ja512:4.3_0": "Kumbhassa vajjāni anappakāni,",
  "ja512:4.4_0": "Dose bahū kumbhagate suṇātha.",
  "ja512:5.1_0": "Gaḷeyya yaṁ pitvā pate papātaṁ,",
  "ja512:5.2_0": "Sobbhaṁ guhaṁ candaniyoḷigallaṁ;",
  "ja512:5.3_0": "Bahumpi bhuñjeyya abhojaneyyaṁ,",
  "ja512:5.4_0": "Tassā puṇṇaṁ kumbhamimaṁ kiṇātha.",
  "ja512:6.1_0": "Yaṁ pitva cittasmimanesamāno,",
  "ja512:6.2_0": "Āhiṇḍatī goriva bhakkhasādī;",
  "ja512:6.3_0": "Anāthamāno upagāyati naccati ca,",
  "ja512:6.4_0": "Tassā puṇṇaṁ kumbhamimaṁ kiṇātha.",
  "ja512:7.1_0": "Yaṁ ve pivitvā acelova naggo,",
  "ja512:7.2_0": "Careyya gāme visikhantarāni;",
  "ja512:7.3_0": "Sammūḷhacitto ativelasāyī,",
  "ja512:7.4_0": "Tassā puṇṇaṁ kumbhamimaṁ kiṇātha.",
  "ja512:8.1_0": "Yaṁ pitva uṭṭhāya pavedhamāno,",
  "ja512:8.2_0": "Sīsañca bāhuñca pacālayanto;",
  "ja512:8.3_0": "So naccatī dārukaṭallakova,",
  "ja512:8.4_0": "Tassā puṇṇaṁ kumbhamimaṁ kiṇātha.",
  "ja512:9.1_0": "Yaṁ ve pivitvā aggidaḍḍhā sayanti,",
  "ja512:9.2_0": "Atho sigālehipi khāditāse;",
  "ja512:9.3_0": "Bandhaṁ vadhaṁ bhogajāniñcupenti,",
  "ja512:9.4_0": "Tassā puṇṇaṁ kumbhamimaṁ kiṇātha.",
  "ja512:10.1_0": "Yaṁ pitva bhāseyya abhāsaneyyaṁ,",
  "ja512:10.2_0": "Sabhāyamāsīno apetavattho;",
  "ja512:10.3_0": "Sammakkhito vantagato byasanno,",
  "ja512:10.4_0": "Tassā puṇṇaṁ kumbhamimaṁ kiṇātha.",
  "ja512:11.1_0": "Yaṁ ve pivitvā ukkaṭṭho āvilakkho,",
  "ja512:11.2_0": "Mameva sabbā pathavīti maññe;",
  "ja512:11.3_0": "Na me samo cāturantopi rājā,",
  "ja512:11.4_0": "Tassā puṇṇaṁ kumbhamimaṁ kiṇātha.",
  "ja512:12.1_0": "Mānātimānā kalahāni pesuṇī,",
  "ja512:12.2_0": "Dubbaṇṇinī naggayinī palāyinī;",
  "ja512:12.3_0": "Corāna dhuttāna gatī niketo,",
  "ja512:12.4_0": "Tassā puṇṇaṁ kumbhamimaṁ kiṇātha.",
  "ja512:13.1_0": "Iddhāni phītāni kulāni assu,",
  "ja512:13.2_0": "Anekasāhassadhanāni loke;",
  "ja512:13.3_0": "Ucchinnadāyajjakatānimāya,",
  "ja512:13.4_0": "Tassā puṇṇaṁ kumbhamimaṁ kiṇātha.",
  "ja512:14.1_0": "Dhaññaṁ dhanaṁ rajataṁ jātarūpaṁ,",
  "ja512:14.2_0": "Khettaṁ gavaṁ yattha vināsayanti;",
  "ja512:14.3_0": "Ucchedanī vittavataṁ kulānaṁ,",
  "ja512:14.4_0": "Tassā puṇṇaṁ kumbhamimaṁ kiṇātha.",
  "ja512:15.1_0": "Yaṁ ve pitvā dittarūpova poso,",
  "ja512:15.2_0": "Akkosati mātaraṁ pitarañca;",
  "ja512:15.3_0": "Sassumpi gaṇheyya athopi suṇhaṁ,",
  "ja512:15.4_0": "Tassā puṇṇaṁ kumbhamimaṁ kiṇātha.",
  "ja512:16.1_0": "Yaṁ ve pitvā dittarūpāva nārī,",
  "ja512:16.2_0": "Akkosatī sasuraṁ sāmikañca;",
  "ja512:16.3_0": "Dāsampi gaṇhe paricārakampi,",
  "ja512:16.4_0": "Tassā puṇṇaṁ kumbhamimaṁ kiṇātha.",
  "ja512:17.1_0": "Yaṁ ve pivitvāna haneyya poso,",
  "ja512:17.2_0": "Dhamme ṭhitaṁ samaṇaṁ brāhmaṇaṁ vā;",
  "ja512:17.3_0": "Gacche apāyampi tatonidānaṁ,",
  "ja512:17.4_0": "Tassā puṇṇaṁ kumbhamimaṁ kiṇātha.",
  "ja512:18.1_0": "Yaṁ ve pivitvā duccaritaṁ caranti,",
  "ja512:18.2_0": "Kāyena vācāya ca cetasā ca;",
  "ja512:18.3_0": "Nirayaṁ vajanti duccaritaṁ caritvā,",
  "ja512:18.4_0": "Tassā puṇṇaṁ kumbhamimaṁ kiṇātha.",
  "ja512:19.1_0": "Yaṁ yācamānā na labhanti pubbe,",
  "ja512:19.2_0": "Bahuṁ hiraññampi pariccajantā;",
  "ja512:19.3_0": "So taṁ pivitvā alikaṁ bhaṇāti,",
  "ja512:19.4_0": "Tassā puṇṇaṁ kumbhamimaṁ kiṇātha.",
  "ja512:20.1_0": "Yaṁ ve pitvā pesane pesiyanto,",
  "ja512:20.2_0": "Accāyike karaṇīyamhi jāte;",
  "ja512:20.3_0": "Atthampi so nappajānāti vutto,",
  "ja512:20.4_0": "Tassā puṇṇaṁ kumbhamimaṁ kiṇātha.",
  "ja512:21.1_0": "Hirīmanāpi ahirīkabhāvaṁ,",
  "ja512:21.2_0": "Pātuṁ karonti madanāya mattā;",
  "ja512:21.3_0": "Dhīrāpi santā bahukaṁ bhaṇanti,",
  "ja512:21.4_0": "Tassā puṇṇaṁ kumbhamimaṁ kiṇātha.",
  "ja512:22.1_0": "Yaṁ ve pitvā ekathūpā sayanti,",
  "ja512:22.2_0": "Anāsakā thaṇḍiladukkhaseyyaṁ;",
  "ja512:22.3_0": "Dubbaṇṇiyaṁ āyasakyañcupenti,",
  "ja512:22.4_0": "Tassā puṇṇaṁ kumbhamimaṁ kiṇātha.",
  "ja512:23.1_0": "Yaṁ ve pitvā pattakhandhā sayanti,",
  "ja512:23.2_0": "Gāvo kuṭahatāva na hi vāruṇiyā;",
  "ja512:23.3_0": "Vego narena susahoriva,",
  "ja512:23.4_0": "Tassā puṇṇaṁ kumbhamimaṁ kiṇātha.",
  "ja512:24.1_0": "Yaṁ manussā vivajjenti,",
  "ja512:24.2_0": "sappaṁ ghoravisamiva;",
  "ja512:24.3_0": "Taṁ loke visasamānaṁ,",
  "ja512:24.4_0": "ko naro pātumarahati.",
  "ja512:25.1_0": "Yaṁ ve pitvā andhakaveṇḍaputtā,",
  "ja512:25.2_0": "Samuddatīre paricārayantā;",
  "ja512:25.3_0": "Upakkamuṁ musalehaññamaññaṁ,",
  "ja512:25.4_0": "Tassā puṇṇaṁ kumbhamimaṁ kiṇātha.",
  "ja512:26.1_0": "Yaṁ ve pitvā pubbadevā pamattā,",
  "ja512:26.2_0": "Tidivā cutā sassatiyā samāya;",
  "ja512:26.3_0": "Taṁ tādisaṁ majjamimaṁ niratthakaṁ,",
  "ja512:26.4_0": "Jānaṁ mahārāja kathaṁ piveyya.",
  "ja512:27.1_0": "Nayimasmiṁ kumbhasmiṁ dadhi vā madhu vā,",
  "ja512:27.2_0": "Evaṁ abhiññāya kiṇāhi rāja;",
  "ja512:27.3_0": "Evañhimaṁ kumbhagatā mayā te,",
  "ja512:27.4_0": "Akkhātarūpaṁ tava sabbamitta”.",
  "ja512:28.1_0": "“Na me pitā vā athavāpi mātā,",
  "ja512:28.2_0": "Etādisā yādisako tuvaṁsi;",
  "ja512:28.3_0": "Hitānukampī paramatthakāmo,",
  "ja512:28.4_0": "Sohaṁ karissaṁ vacanaṁ tavajja.",
  "ja512:29.1_0": "Dadāmi te gāmavarāni pañca,",
  "ja512:29.2_0": "Dāsīsataṁ satta gavaṁsatāni;",
  "ja512:29.3_0": "Ājaññayutte ca rathe dasa ime,",
  "ja512:29.4_0": "Ācariyo hosi mamatthakāmo”.",
  "ja512:30.1_0": "“Taveva dāsīsatamatthu rāja,",
  "ja512:30.2_0": "Gāmā ca gāvo ca taveva hontu;",
  "ja512:30.3_0": "Ājaññayuttā ca rathā taveva,",
  "ja512:30.4_0": "Sakkohamasmī tidasānamindo.",
  "ja512:31.1_0": "Maṁsodanaṁ sappipāyāsaṁ bhuñja,",
  "ja512:31.2_0": "Khādassu ca tvaṁ madhumāsapūve;",
  "ja512:31.3_0": "Evaṁ tuvaṁ dhammarato janinda,",
  "ja512:31.4_0": "Anindito saggamupehi ṭhānan”ti.",
  "ja512:32.1_0": "Kumbhajātakaṁ dutiyaṁ."
}