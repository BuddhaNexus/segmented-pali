{
  "tha-ap540:0.1_0": "Therāpadāna",
  "tha-ap540:0.2_0": "Kaccāyanavagga",
  "tha-ap540:0.3_0": "8 Uruveḷakassapattheraapadāna",
  "tha-ap540:1.1_0": "“Padumuttaro nāma jino,",
  "tha-ap540:1.2_0": "sabbalokavidū muni;",
  "tha-ap540:1.3_0": "Ito satasahassamhi,",
  "tha-ap540:1.4_0": "kappe uppajji cakkhumā.",
  "tha-ap540:2.1_0": "Ovādako viññāpako,",
  "tha-ap540:2.2_0": "tārako sabbapāṇinaṁ;",
  "tha-ap540:2.3_0": "Desanākusalo buddho,",
  "tha-ap540:2.4_0": "tāresi janataṁ bahuṁ.",
  "tha-ap540:3.1_0": "Anukampako kāruṇiko,",
  "tha-ap540:3.2_0": "hitesī sabbapāṇinaṁ;",
  "tha-ap540:3.3_0": "Sampatte titthiye sabbe,",
  "tha-ap540:3.4_0": "pañcasīle patiṭṭhapi.",
  "tha-ap540:4.1_0": "Evaṁ nirākulaṁ āsi,",
  "tha-ap540:4.2_0": "suññataṁ titthiyehi ca;",
  "tha-ap540:4.3_0": "Vicittaṁ arahantehi,",
  "tha-ap540:4.4_0": "vasībhūtehi tādibhi.",
  "tha-ap540:5.1_0": "Ratanānaṭṭhapaññāsaṁ,",
  "tha-ap540:5.2_0": "uggato so mahāmuni;",
  "tha-ap540:5.3_0": "Kañcanagghiyasaṅkāso,",
  "tha-ap540:5.4_0": "bāttiṁsavaralakkhaṇo.",
  "tha-ap540:6.1_0": "Vassasatasahassāni,",
  "tha-ap540:6.2_0": "āyu vijjati tāvade;",
  "tha-ap540:6.3_0": "Tāvatā tiṭṭhamāno so,",
  "tha-ap540:6.4_0": "tāresi janataṁ bahuṁ.",
  "tha-ap540:7.1_0": "Tadāhaṁ haṁsavatiyā,",
  "tha-ap540:7.2_0": "brāhmaṇo sādhusammato;",
  "tha-ap540:7.3_0": "Upecca lokapajjotaṁ,",
  "tha-ap540:7.4_0": "assosiṁ dhammadesanaṁ.",
  "tha-ap540:8.1_0": "Tadā mahāparisatiṁ,",
  "tha-ap540:8.2_0": "mahāparisasāvakaṁ;",
  "tha-ap540:8.3_0": "Ṭhapentaṁ etadaggamhi,",
  "tha-ap540:8.4_0": "sutvāna mudito ahaṁ.",
  "tha-ap540:9.1_0": "Mahatā parivārena,",
  "tha-ap540:9.2_0": "nimantetvā mahājinaṁ;",
  "tha-ap540:9.3_0": "Brāhmaṇānaṁ sahassena,",
  "tha-ap540:9.4_0": "sahadānamadāsahaṁ.",
  "tha-ap540:10.1_0": "Mahādānaṁ daditvāna,",
  "tha-ap540:10.2_0": "abhivādiya nāyakaṁ;",
  "tha-ap540:10.3_0": "Ekamantaṁ ṭhito haṭṭho,",
  "tha-ap540:10.4_0": "idaṁ vacanamabraviṁ.",
  "tha-ap540:11.1_0": "‘Tayi saddhāya me vīra,",
  "tha-ap540:11.2_0": "adhikāraguṇena ca;",
  "tha-ap540:11.3_0": "Parisā mahatī hotu,",
  "tha-ap540:11.4_0": "nibbattassa tahiṁ tahiṁ’.",
  "tha-ap540:12.1_0": "Tadā avoca parisaṁ,",
  "tha-ap540:12.2_0": "gajagajjitasussaro;",
  "tha-ap540:12.3_0": "Karavīkaruto satthā,",
  "tha-ap540:12.4_0": "‘etaṁ passatha brāhmaṇaṁ.",
  "tha-ap540:13.1_0": "Hemavaṇṇaṁ mahābāhuṁ,",
  "tha-ap540:13.2_0": "kamalānanalocanaṁ;",
  "tha-ap540:13.3_0": "Udaggatanujaṁ haṭṭhaṁ,",
  "tha-ap540:13.4_0": "saddhavantaṁ guṇe mama.",
  "tha-ap540:14.1_0": "Esa patthayate ṭhānaṁ,",
  "tha-ap540:14.2_0": "sīhaghosassa bhikkhuno;",
  "tha-ap540:14.3_0": "Anāgatamhi addhāne,",
  "tha-ap540:14.4_0": "lacchase taṁ manorathaṁ.",
  "tha-ap540:15.1_0": "Satasahassito kappe,",
  "tha-ap540:15.2_0": "okkākakulasambhavo;",
  "tha-ap540:15.3_0": "Gotamo nāma gottena,",
  "tha-ap540:15.4_0": "satthā loke bhavissati.",
  "tha-ap540:16.1_0": "Tassa dhammesu dāyādo,",
  "tha-ap540:16.2_0": "oraso dhammanimmito;",
  "tha-ap540:16.3_0": "Kassapo nāma gottena,",
  "tha-ap540:16.4_0": "hessati satthu sāvako’.",
  "tha-ap540:17.1_0": "Ito dvenavute kappe,",
  "tha-ap540:17.2_0": "ahu satthā anuttaro;",
  "tha-ap540:17.3_0": "Anūpamo asadiso,",
  "tha-ap540:17.4_0": "phusso lokagganāyako.",
  "tha-ap540:18.1_0": "So ca sabbaṁ tamaṁ hantvā,",
  "tha-ap540:18.2_0": "vijaṭetvā mahājaṭaṁ;",
  "tha-ap540:18.3_0": "Vassate amataṁ vuṭṭhiṁ,",
  "tha-ap540:18.4_0": "tappayanto sadevakaṁ.",
  "tha-ap540:19.1_0": "Tadā hi bārāṇasiyaṁ,",
  "tha-ap540:19.2_0": "rājā paccā ahumhase;",
  "tha-ap540:19.3_0": "Bhātaromha tayo sabbe,",
  "tha-ap540:19.4_0": "saṁvisaṭṭhāva rājino.",
  "tha-ap540:20.1_0": "Vīraṅgarūpā balino,",
  "tha-ap540:20.2_0": "saṅgāme aparājitā;",
  "tha-ap540:20.3_0": "Tadā kupitapaccanto,",
  "tha-ap540:20.4_0": "amhe āha mahīpati.",
  "tha-ap540:21.1_0": "‘Etha gantvāna paccantaṁ,",
  "tha-ap540:21.2_0": "sodhetvā aṭṭavībalaṁ;",
  "tha-ap540:21.3_0": "Khemaṁ vijiritaṁ katvā,",
  "tha-ap540:21.4_0": "puna dethāti bhāsatha’.",
  "tha-ap540:22.1_0": "Tato mayaṁ avocumha,",
  "tha-ap540:22.2_0": "yadi deyyāsi nāyakaṁ;",
  "tha-ap540:22.3_0": "Upaṭṭhānāya amhākaṁ,",
  "tha-ap540:22.4_0": "sādhayissāma vo tato.",
  "tha-ap540:23.1_0": "Tato mayaṁ laddhavarā,",
  "tha-ap540:23.2_0": "bhūmipālena pesitā;",
  "tha-ap540:23.3_0": "Nikkhittasatthaṁ paccantaṁ,",
  "tha-ap540:23.4_0": "katvā punarupacca taṁ.",
  "tha-ap540:24.1_0": "Yācitvā satthupaṭṭhānaṁ,",
  "tha-ap540:24.2_0": "rājānaṁ lokanāyakaṁ;",
  "tha-ap540:24.3_0": "Munivīraṁ labhitvāna,",
  "tha-ap540:24.4_0": "yāvajīvaṁ yajimha taṁ.",
  "tha-ap540:25.1_0": "Mahagghāni ca vatthāni,",
  "tha-ap540:25.2_0": "paṇītāni rasāni ca;",
  "tha-ap540:25.3_0": "Senāsanāni rammāni,",
  "tha-ap540:25.4_0": "bhesajjāni hitāni ca.",
  "tha-ap540:26.1_0": "Datvā sasaṅghamunino,",
  "tha-ap540:26.2_0": "dhammenuppāditāni no;",
  "tha-ap540:26.3_0": "Sīlavanto kāruṇikā,",
  "tha-ap540:26.4_0": "bhāvanāyuttamānasā.",
  "tha-ap540:27.1_0": "Saddhā paricaritvāna,",
  "tha-ap540:27.2_0": "mettacittena nāyakaṁ;",
  "tha-ap540:27.3_0": "Nibbute tamhi lokagge,",
  "tha-ap540:27.4_0": "pūjaṁ katvā yathābalaṁ.",
  "tha-ap540:28.1_0": "Tato cutā santusitaṁ,",
  "tha-ap540:28.2_0": "gatā tattha mahāsukhaṁ;",
  "tha-ap540:28.3_0": "Anubhūtā mayaṁ sabbe,",
  "tha-ap540:28.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap540:29.1_0": "Māyākāro yathā raṅge,",
  "tha-ap540:29.2_0": "dassesi vikatiṁ bahuṁ;",
  "tha-ap540:29.3_0": "Tathā bhave bhamantohaṁ,",
  "tha-ap540:29.4_0": "videhādhipatī ahuṁ.",
  "tha-ap540:30.1_0": "Guṇāceḷassa vākyena,",
  "tha-ap540:30.2_0": "micchādiṭṭhigatāsayo;",
  "tha-ap540:30.3_0": "Narakaṁ maggamārūḷho,",
  "tha-ap540:30.4_0": "rucāya mama dhītuyā.",
  "tha-ap540:31.1_0": "Ovādaṁ nādiyitvāna,",
  "tha-ap540:31.2_0": "brahmunā nāradenahaṁ;",
  "tha-ap540:31.3_0": "Bahudhā saṁsito santo,",
  "tha-ap540:31.4_0": "diṭṭhiṁ hitvāna pāpikaṁ.",
  "tha-ap540:32.1_0": "Pūrayitvā visesena,",
  "tha-ap540:32.2_0": "dasa kammapathānihaṁ;",
  "tha-ap540:32.3_0": "Hitvāna dehamagamiṁ,",
  "tha-ap540:32.4_0": "saggaṁ sabhavanaṁ yathā.",
  "tha-ap540:33.1_0": "Pacchime bhave sampatte,",
  "tha-ap540:33.2_0": "brahmabandhu ahosahaṁ;",
  "tha-ap540:33.3_0": "Bārāṇasiyaṁ phītāyaṁ,",
  "tha-ap540:33.4_0": "jāto vippamahākule.",
  "tha-ap540:34.1_0": "Maccubyādhijarā bhīto,",
  "tha-ap540:34.2_0": "ogāhetvā mahāvanaṁ;",
  "tha-ap540:34.3_0": "Nibbānaṁ padamesanto,",
  "tha-ap540:34.4_0": "jaṭilesu paribbajiṁ.",
  "tha-ap540:35.1_0": "Tadā dve bhātaro mayhaṁ,",
  "tha-ap540:35.2_0": "pabbajiṁsu mayā saha;",
  "tha-ap540:35.3_0": "Uruvelāyaṁ māpetvā,",
  "tha-ap540:35.4_0": "assamaṁ nivasiṁ ahaṁ.",
  "tha-ap540:36.1_0": "Kassapo nāma gottena,",
  "tha-ap540:36.2_0": "uruveḷanivāsiko;",
  "tha-ap540:36.3_0": "Tato me āsi paññatti,",
  "tha-ap540:36.4_0": "uruveḷakassapo iti.",
  "tha-ap540:37.1_0": "Nadīsakāse bhātā me,",
  "tha-ap540:37.2_0": "nadīkassapasavhayo;",
  "tha-ap540:37.3_0": "Āsī sakāsanāmena,",
  "tha-ap540:37.4_0": "gayāyaṁ gayākassapo.",
  "tha-ap540:38.1_0": "Dve satāni kaniṭṭhassa,",
  "tha-ap540:38.2_0": "tīṇi majjhassa bhātuno;",
  "tha-ap540:38.3_0": "Mama pañca satānūnā,",
  "tha-ap540:38.4_0": "sissā sabbe mamānugā.",
  "tha-ap540:39.1_0": "Tadā upecca maṁ buddho,",
  "tha-ap540:39.2_0": "katvāna vividhāni me;",
  "tha-ap540:39.3_0": "Pāṭihīrāni lokaggo,",
  "tha-ap540:39.4_0": "vinesi narasārathi.",
  "tha-ap540:40.1_0": "Sahassaparivārena,",
  "tha-ap540:40.2_0": "ahosiṁ ehibhikkhuko;",
  "tha-ap540:40.3_0": "Teheva saha sabbehi,",
  "tha-ap540:40.4_0": "arahattamapāpuṇiṁ.",
  "tha-ap540:41.1_0": "Te cevaññe ca bahavo,",
  "tha-ap540:41.2_0": "sissā maṁ parivārayuṁ;",
  "tha-ap540:41.3_0": "Sāsituñca samatthohaṁ,",
  "tha-ap540:41.4_0": "tato maṁ isisattamo.",
  "tha-ap540:42.1_0": "Mahāparisabhāvasmiṁ,",
  "tha-ap540:42.2_0": "etadagge ṭhapesi maṁ;",
  "tha-ap540:42.3_0": "Aho buddhe kataṁ kāraṁ,",
  "tha-ap540:42.4_0": "saphalaṁ me ajāyatha.",
  "tha-ap540:43.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap540:43.2_0": "…pe…",
  "tha-ap540:43.3_0": "viharāmi anāsavo.",
  "tha-ap540:44.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap540:44.2_0": "…pe…",
  "tha-ap540:44.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap540:45.1_0": "Paṭisambhidā catasso,",
  "tha-ap540:45.2_0": "…pe…",
  "tha-ap540:45.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap540:46.1_0": "Itthaṁ sudaṁ āyasmā uruveḷakassapo thero imā gāthāyo abhāsitthāti.",
  "tha-ap540:47.1_0": "Uruveḷakassapattherassāpadānaṁ aṭṭhamaṁ."
}