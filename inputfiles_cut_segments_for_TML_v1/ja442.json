{
  "ja442:0.1_0": "Jātaka",
  "ja442:0.2_0": "Dasakanipāta",
  "ja442:0.3_0": "Catudvāravagga",
  "ja442:0.4_0": "4. Saṅkhajātaka",
  "ja442:1.1_0": "“Bahussuto sutadhammosi saṅkha,",
  "ja442:1.2_0": "Diṭṭhā tayā samaṇabrāhmaṇā ca;",
  "ja442:1.3_0": "Athakkhaṇe dassayase vilāpaṁ,",
  "ja442:1.4_0": "Añño nu ko te paṭimantako mayā”.",
  "ja442:2.1_0": "“Subbhū subhā suppaṭimukkakambu,",
  "ja442:2.2_0": "Paggayha sovaṇṇamayāya pātiyā;",
  "ja442:2.3_0": "‘Bhuñjassu bhattaṁ’ iti maṁ vadeti,",
  "ja442:2.4_0": "Saddhāvittā",
  "ja442:2.5_0": "Tamahaṁ noti brūmi”.",
  "ja442:3.1_0": "“Etādisaṁ brāhmaṇa disvāna yakkhaṁ,",
  "ja442:3.2_0": "Puccheyya poso sukhamāsisāno;",
  "ja442:3.3_0": "Uṭṭhehi naṁ pañjalikābhipuccha,",
  "ja442:3.4_0": "Devī nusi tvaṁ uda mānusī nu”.",
  "ja442:4.1_0": "“Yaṁ tvaṁ sukhenābhisamekkhase maṁ,",
  "ja442:4.2_0": "Bhuñjassu bhattaṁ iti maṁ vadesi;",
  "ja442:4.3_0": "Pucchāmi taṁ nāri mahānubhāve,",
  "ja442:4.4_0": "Devī nusi tvaṁ uda mānusī nu”.",
  "ja442:5.1_0": "“Devī ahaṁ saṅkha mahānubhāvā,",
  "ja442:5.2_0": "Idhāgatā sāgaravārimajjhe;",
  "ja442:5.3_0": "Anukampikā no ca paduṭṭhacittā,",
  "ja442:5.4_0": "Taveva atthāya idhāgatāsmi.",
  "ja442:6.1_0": "Idhannapānaṁ sayanāsanañca,",
  "ja442:6.2_0": "Yānāni nānāvividhāni saṅkha;",
  "ja442:6.3_0": "Sabbassa tyāhaṁ paṭipādayāmi,",
  "ja442:6.4_0": "Yaṁ kiñci tuyhaṁ manasābhipatthitaṁ”.",
  "ja442:7.1_0": "“Yaṁ kiñci yiṭṭhañca hutañca mayhaṁ,",
  "ja442:7.2_0": "Sabbassa no issarā tvaṁ sugatte;",
  "ja442:7.3_0": "Susoṇi subbhamu suvilaggamajjhe,",
  "ja442:7.4_0": "Kissa me kammassa ayaṁ vipāko”.",
  "ja442:8.1_0": "“Ghamme pathe brāhmaṇa ekabhikkhuṁ,",
  "ja442:8.2_0": "Ugghaṭṭapādaṁ tasitaṁ kilantaṁ;",
  "ja442:8.3_0": "Paṭipādayī saṅkha upāhanāni,",
  "ja442:8.4_0": "Sā dakkhiṇā kāmaduhā tavajja”.",
  "ja442:9.1_0": "“Sā hotu nāvā phalakūpapannā,",
  "ja442:9.2_0": "Anavassutā erakavātayuttā;",
  "ja442:9.3_0": "Aññassa yānassa na hettha bhūmi,",
  "ja442:9.4_0": "Ajjeva maṁ moḷiniṁ pāpayassu”.",
  "ja442:10.1_0": "“Sā tattha vittā sumanā patītā,",
  "ja442:10.2_0": "Nāvaṁ sucittaṁ abhinimminitvā;",
  "ja442:10.3_0": "Ādāya saṅkhaṁ purisena saddhiṁ,",
  "ja442:10.4_0": "Upānayī nagaraṁ sādhuramman”ti.",
  "ja442:11.1_0": "Saṅkhajātakaṁ catutthaṁ."
}