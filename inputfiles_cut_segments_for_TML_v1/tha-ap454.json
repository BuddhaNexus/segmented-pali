{
  "tha-ap454:0.1_0": "Therāpadāna",
  "tha-ap454:0.2_0": "Jagatidāyakavagga",
  "tha-ap454:0.3_0": "2. Morahatthiyattheraapadāna",
  "tha-ap454:1.1_0": "“Morahatthaṁ gahetvāna,",
  "tha-ap454:1.2_0": "upesiṁ lokanāyakaṁ;",
  "tha-ap454:1.3_0": "Pasannacitto sumano,",
  "tha-ap454:1.4_0": "morahatthamadāsahaṁ.",
  "tha-ap454:2.1_0": "Iminā morahatthena,",
  "tha-ap454:2.2_0": "cetanāpaṇidhīhi ca;",
  "tha-ap454:2.3_0": "Nibbāyiṁsu tayo aggī,",
  "tha-ap454:2.4_0": "labhāmi vipulaṁ sukhaṁ.",
  "tha-ap454:3.1_0": "Aho buddhā aho dhammā,",
  "tha-ap454:3.2_0": "aho no satthusampadā;",
  "tha-ap454:3.3_0": "Datvānahaṁ morahatthaṁ,",
  "tha-ap454:3.4_0": "labhāmi vipulaṁ sukhaṁ.",
  "tha-ap454:4.1_0": "Tiyaggī nibbutā mayhaṁ,",
  "tha-ap454:4.2_0": "bhavā sabbe samūhatā;",
  "tha-ap454:4.3_0": "Sabbāsavā parikkhīṇā,",
  "tha-ap454:4.4_0": "natthi dāni punabbhavo.",
  "tha-ap454:5.1_0": "Ekatiṁse ito kappe,",
  "tha-ap454:5.2_0": "yaṁ dānamadadiṁ tadā;",
  "tha-ap454:5.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap454:5.4_0": "morahatthassidaṁ phalaṁ.",
  "tha-ap454:6.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap454:6.2_0": "…pe…",
  "tha-ap454:6.3_0": "viharāmi anāsavo.",
  "tha-ap454:7.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap454:7.2_0": "…pe…",
  "tha-ap454:7.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap454:8.1_0": "Paṭisambhidā catasso,",
  "tha-ap454:8.2_0": "…pe…",
  "tha-ap454:8.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap454:9.1_0": "Itthaṁ sudaṁ āyasmā morahatthiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap454:10.1_0": "Morahatthiyattherassāpadānaṁ dutiyaṁ."
}