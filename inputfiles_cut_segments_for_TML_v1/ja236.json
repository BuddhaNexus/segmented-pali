{
  "ja236:0.1_0": "Jātaka",
  "ja236:0.2_0": "Dukanipāta",
  "ja236:0.3_0": "Upāhanavagga",
  "ja236:0.4_0": "6. Bakajātaka",
  "ja236:1.1_0": "“Bhaddako vatayaṁ pakkhī,",
  "ja236:1.2_0": "dijo kumudasannibho;",
  "ja236:1.3_0": "Vūpasantehi pakkhehi,",
  "ja236:1.4_0": "mandamandova jhāyati”.",
  "ja236:2.1_0": "“Nāssa sīlaṁ vijānātha,",
  "ja236:2.2_0": "anaññāya pasaṁsatha;",
  "ja236:2.3_0": "Amhe dijo na pāleti,",
  "ja236:2.4_0": "tena pakkhī na phandatī”ti.",
  "ja236:3.1_0": "Bakajātakaṁ chaṭṭhaṁ."
}