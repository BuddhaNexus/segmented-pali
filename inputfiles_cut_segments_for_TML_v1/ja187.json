{
  "ja187:0.1_0": "Jātaka",
  "ja187:0.2_0": "Dukanipāta",
  "ja187:0.3_0": "Asadisavagga",
  "ja187:0.4_0": "7. Catumaṭṭhajātaka",
  "ja187:1.1_0": "“Ucce viṭabhimāruyha,",
  "ja187:1.2_0": "mantayavho rahogatā;",
  "ja187:1.3_0": "Nīce oruyha mantavho,",
  "ja187:1.4_0": "migarājāpi sossati.",
  "ja187:2.1_0": "Yaṁ suvaṇṇo suvaṇṇena,",
  "ja187:2.2_0": "devo devena mantaye;",
  "ja187:2.3_0": "Kiṁ tettha catumaṭṭhassa,",
  "ja187:2.4_0": "bilaṁ pavisa jambukā”ti.",
  "ja187:3.1_0": "Catumaṭṭhajātakaṁ sattamaṁ."
}