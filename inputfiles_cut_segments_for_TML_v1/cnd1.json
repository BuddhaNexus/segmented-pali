{
  "cnd1:0.1_0": "Cūḷaniddesa",
  "cnd1:0.2_0": "Pārāyanavaggagāthā",
  "cnd1:0.3_0": "1. Vatthugāthā",
  "cnd1:0.4_0": "Namo tassa Bhagavato Arahato Sammāsambuddhassa.",
  "cnd1:1.1_0": "Kosalānaṁ purā rammā,",
  "cnd1:1.2_0": "agamā dakkhiṇāpathaṁ;",
  "cnd1:1.3_0": "Ākiñcaññaṁ patthayāno,",
  "cnd1:1.4_0": "brāhmaṇo mantapāragū.",
  "cnd1:2.1_0": "So assakassa visaye,",
  "cnd1:2.2_0": "maḷakassa samāsane;",
  "cnd1:2.3_0": "Vasi godhāvarīkūle,",
  "cnd1:2.4_0": "uñchena ca phalena ca.",
  "cnd1:3.1_0": "Tasseva upanissāya,",
  "cnd1:3.2_0": "gāmo ca vipulo ahu;",
  "cnd1:3.3_0": "Tato jātena āyena,",
  "cnd1:3.4_0": "mahāyaññamakappayi.",
  "cnd1:4.1_0": "Mahāyaññaṁ yajitvāna,",
  "cnd1:4.2_0": "puna pāvisi assamaṁ;",
  "cnd1:4.3_0": "Tasmiṁ paṭipaviṭṭhamhi,",
  "cnd1:4.4_0": "añño āgañchi brāhmaṇo.",
  "cnd1:5.1_0": "Ugghaṭṭapādo tasito,",
  "cnd1:5.2_0": "paṅkadanto rajassiro;",
  "cnd1:5.3_0": "So ca naṁ upasaṅkamma,",
  "cnd1:5.4_0": "satāni pañca yācati.",
  "cnd1:6.1_0": "Tamenaṁ bāvarī disvā,",
  "cnd1:6.2_0": "āsanena nimantayi;",
  "cnd1:6.3_0": "Sukhañca kusalaṁ pucchi,",
  "cnd1:6.4_0": "idaṁ vacanamabravi.",
  "cnd1:7.1_0": "“Yaṁ kho mama deyyadhammaṁ,",
  "cnd1:7.2_0": "Sabbaṁ visajjitaṁ mayā;",
  "cnd1:7.3_0": "Anujānāhi me brahme,",
  "cnd1:7.4_0": "Natthi pañcasatāni me”.",
  "cnd1:8.1_0": "“Sace me yācamānassa,",
  "cnd1:8.2_0": "bhavaṁ nānupadassati;",
  "cnd1:8.3_0": "Sattame divase tuyhaṁ,",
  "cnd1:8.4_0": "muddhā phalatu sattadhā”.",
  "cnd1:9.1_0": "Abhisaṅkharitvā kuhako,",
  "cnd1:9.2_0": "bheravaṁ so akittayi;",
  "cnd1:9.3_0": "Tassa taṁ vacanaṁ sutvā,",
  "cnd1:9.4_0": "bāvarī dukkhito ahu.",
  "cnd1:10.1_0": "Ussussati anāhāro,",
  "cnd1:10.2_0": "sokasallasamappito;",
  "cnd1:10.3_0": "Athopi evaṁ cittassa,",
  "cnd1:10.4_0": "jhāne na ramatī mano.",
  "cnd1:11.1_0": "Utrastaṁ dukkhitaṁ disvā,",
  "cnd1:11.2_0": "devatā atthakāminī;",
  "cnd1:11.3_0": "Bāvariṁ upasaṅkamma,",
  "cnd1:11.4_0": "idaṁ vacanamabravi.",
  "cnd1:12.1_0": "“Na so muddhaṁ pajānāti,",
  "cnd1:12.2_0": "kuhako so dhanatthiko;",
  "cnd1:12.3_0": "Muddhani muddhapāte vā,",
  "cnd1:12.4_0": "ñāṇaṁ tassa na vijjati”.",
  "cnd1:13.1_0": "“Bhotī carahi jānāti,",
  "cnd1:13.2_0": "Taṁ me akkhāhi pucchitā;",
  "cnd1:13.3_0": "Muddhaṁ muddhādhipātañca,",
  "cnd1:13.4_0": "Taṁ suṇoma vaco tava”.",
  "cnd1:14.1_0": "“Ahampetaṁ na jānāmi,",
  "cnd1:14.2_0": "ñāṇaṁ mettha na vijjati;",
  "cnd1:14.3_0": "Muddhani muddhādhipāte ca,",
  "cnd1:14.4_0": "jinānaṁ hettha dassanaṁ”.",
  "cnd1:15.1_0": "“Atha ko carahi jānāti,",
  "cnd1:15.2_0": "asmiṁ pathavimaṇḍale;",
  "cnd1:15.3_0": "Muddhaṁ muddhādhipātañca,",
  "cnd1:15.4_0": "taṁ me akkhāhi devate”.",
  "cnd1:16.1_0": "“Purā kapilavatthumhā,",
  "cnd1:16.2_0": "nikkhanto lokanāyako;",
  "cnd1:16.3_0": "Apacco okkākarājassa,",
  "cnd1:16.4_0": "sakyaputto pabhaṅkaro.",
  "cnd1:17.1_0": "So hi brāhmaṇa sambuddho,",
  "cnd1:17.2_0": "sabbadhammāna pāragū;",
  "cnd1:17.3_0": "Sabbābhiññābalappatto,",
  "cnd1:17.4_0": "sabbadhammesu cakkhumā;",
  "cnd1:17.5_0": "Sabbakammakkhayaṁ patto,",
  "cnd1:17.6_0": "vimutto upadhikkhaye.",
  "cnd1:18.1_0": "Buddho so bhagavā loke,",
  "cnd1:18.2_0": "dhammaṁ deseti cakkhumā;",
  "cnd1:18.3_0": "Taṁ tvaṁ gantvāna pucchassu,",
  "cnd1:18.4_0": "so te taṁ byākarissati”.",
  "cnd1:19.1_0": "Sambuddhoti vaco sutvā,",
  "cnd1:19.2_0": "udaggo bāvarī ahu;",
  "cnd1:19.3_0": "Sokassa tanuko āsi,",
  "cnd1:19.4_0": "pītiñca vipulaṁ labhi.",
  "cnd1:20.1_0": "So bāvarī attamano udaggo,",
  "cnd1:20.2_0": "Taṁ devataṁ pucchati vedajāto;",
  "cnd1:20.3_0": "“Katamamhi gāme nigamamhi vā pana,",
  "cnd1:20.4_0": "Katamamhi vā janapade lokanātho;",
  "cnd1:20.5_0": "Yattha gantvāna passemu,",
  "cnd1:20.6_0": "Sambuddhaṁ dvipaduttamaṁ”.",
  "cnd1:21.1_0": "“Sāvatthiyaṁ kosalamandire jino,",
  "cnd1:21.2_0": "Pahūtapañño varabhūrimedhaso;",
  "cnd1:21.3_0": "So sakyaputto vidhuro anāsavo,",
  "cnd1:21.4_0": "Muddhādhipātassa vidū narāsabho”.",
  "cnd1:22.1_0": "Tato āmantayī sisse,",
  "cnd1:22.2_0": "brāhmaṇe mantapāragū;",
  "cnd1:22.3_0": "“Etha māṇavā akkhissaṁ,",
  "cnd1:22.4_0": "suṇātha vacanaṁ mama.",
  "cnd1:23.1_0": "Yasseso dullabho loke,",
  "cnd1:23.2_0": "pātubhāvo abhiṇhaso;",
  "cnd1:23.3_0": "Svājja lokamhi uppanno,",
  "cnd1:23.4_0": "sambuddho iti vissuto;",
  "cnd1:23.5_0": "Khippaṁ gantvāna sāvatthiṁ,",
  "cnd1:23.6_0": "passavho dvipaduttamaṁ”.",
  "cnd1:24.1_0": "“Kathaṁ carahi jānemu,",
  "cnd1:24.2_0": "disvā buddhoti brāhmaṇa;",
  "cnd1:24.3_0": "Ajānataṁ no pabrūhi,",
  "cnd1:24.4_0": "yathā jānemu taṁ mayaṁ”.",
  "cnd1:25.1_0": "“Āgatāni hi mantesu,",
  "cnd1:25.2_0": "mahāpurisalakkhaṇā;",
  "cnd1:25.3_0": "Dvattiṁsāni ca byākkhātā,",
  "cnd1:25.4_0": "samattā anupubbaso.",
  "cnd1:26.1_0": "Yassete honti gattesu,",
  "cnd1:26.2_0": "mahāpurisalakkhaṇā;",
  "cnd1:26.3_0": "Dveyeva tassa gatiyo,",
  "cnd1:26.4_0": "tatiyā hi na vijjati.",
  "cnd1:27.1_0": "Sace agāraṁ āvasati,",
  "cnd1:27.2_0": "vijeyya pathaviṁ imaṁ;",
  "cnd1:27.3_0": "Adaṇḍena asatthena,",
  "cnd1:27.4_0": "dhammena anusāsati.",
  "cnd1:28.1_0": "Sace ca so pabbajati,",
  "cnd1:28.2_0": "Agārā anagāriyaṁ;",
  "cnd1:28.3_0": "Vivaṭṭacchado sambuddho,",
  "cnd1:28.4_0": "Arahā bhavati anuttaro.",
  "cnd1:29.1_0": "Jātiṁ gottañca lakkhaṇaṁ,",
  "cnd1:29.2_0": "mante sisse punāpare;",
  "cnd1:29.3_0": "Muddhaṁ muddhādhipātañca,",
  "cnd1:29.4_0": "manasāyeva pucchatha.",
  "cnd1:30.1_0": "Anāvaraṇadassāvī,",
  "cnd1:30.2_0": "yadi buddho bhavissati;",
  "cnd1:30.3_0": "Manasā pucchite pañhe,",
  "cnd1:30.4_0": "vācāya visajjessati”.",
  "cnd1:31.1_0": "Bāvarissa vaco sutvā,",
  "cnd1:31.2_0": "sissā soḷasa brāhmaṇā;",
  "cnd1:31.3_0": "Ajito tissametteyyo,",
  "cnd1:31.4_0": "puṇṇako atha mettagū.",
  "cnd1:32.1_0": "Dhotako upasīvo ca,",
  "cnd1:32.2_0": "nando ca atha hemako;",
  "cnd1:32.3_0": "Todeyyakappā dubhayo,",
  "cnd1:32.4_0": "jatukaṇṇī ca paṇḍito.",
  "cnd1:33.1_0": "Bhadrāvudho udayo ca,",
  "cnd1:33.2_0": "posālo cāpi brāhmaṇo;",
  "cnd1:33.3_0": "Mogharājā ca medhāvī,",
  "cnd1:33.4_0": "piṅgiyo ca mahāisi.",
  "cnd1:34.1_0": "Paccekagaṇino sabbe,",
  "cnd1:34.2_0": "sabbalokassa vissutā;",
  "cnd1:34.3_0": "Jhāyī jhānaratā dhīrā,",
  "cnd1:34.4_0": "pubbavāsanavāsitā.",
  "cnd1:35.1_0": "Bāvariṁ abhivādetvā,",
  "cnd1:35.2_0": "katvā ca naṁ padakkhiṇaṁ;",
  "cnd1:35.3_0": "Jaṭājinadharā sabbe,",
  "cnd1:35.4_0": "pakkāmuṁ uttarāmukhā.",
  "cnd1:36.1_0": "Maḷakassa patiṭṭhānaṁ,",
  "cnd1:36.2_0": "puramāhissatiṁ tadā;",
  "cnd1:36.3_0": "Ujjeniñcāpi gonaddhaṁ,",
  "cnd1:36.4_0": "vedisaṁ vanasavhayaṁ.",
  "cnd1:37.1_0": "Kosambiñcāpi sāketaṁ,",
  "cnd1:37.2_0": "sāvatthiñca puruttamaṁ;",
  "cnd1:37.3_0": "Setabyaṁ kapilavatthuṁ,",
  "cnd1:37.4_0": "kusinārañca mandiraṁ.",
  "cnd1:38.1_0": "Pāvañca bhoganagaraṁ,",
  "cnd1:38.2_0": "vesāliṁ māgadhaṁ puraṁ;",
  "cnd1:38.3_0": "Pāsāṇakaṁ cetiyañca,",
  "cnd1:38.4_0": "ramaṇīyaṁ manoramaṁ.",
  "cnd1:39.1_0": "Tasitovudakaṁ sītaṁ,",
  "cnd1:39.2_0": "mahālābhaṁva vāṇijo;",
  "cnd1:39.3_0": "Chāyaṁ ghammābhitattova,",
  "cnd1:39.4_0": "turitā pabbatamāruhuṁ.",
  "cnd1:40.1_0": "Bhagavā tamhi samaye,",
  "cnd1:40.2_0": "bhikkhusaṅghapurakkhato;",
  "cnd1:40.3_0": "Bhikkhūnaṁ dhammaṁ deseti,",
  "cnd1:40.4_0": "sīhova nadatī vane.",
  "cnd1:41.1_0": "Ajito addasa buddhaṁ,",
  "cnd1:41.2_0": "Pītaraṁsiṁva bhāṇumaṁ;",
  "cnd1:41.3_0": "Candaṁ yathā pannarase,",
  "cnd1:41.4_0": "Paripūraṁ upāgataṁ.",
  "cnd1:42.1_0": "Athassa gatte disvāna,",
  "cnd1:42.2_0": "paripūrañca byañjanaṁ;",
  "cnd1:42.3_0": "Ekamantaṁ ṭhito haṭṭho,",
  "cnd1:42.4_0": "manopañhe apucchatha.",
  "cnd1:43.1_0": "“Ādissa jammanaṁ brūhi,",
  "cnd1:43.2_0": "gottaṁ brūhi salakkhaṇaṁ;",
  "cnd1:43.3_0": "Mantesu pāramiṁ brūhi,",
  "cnd1:43.4_0": "kati vāceti brāhmaṇo”.",
  "cnd1:44.1_0": "“Vīsaṁ vassasataṁ āyu,",
  "cnd1:44.2_0": "so ca gottena bāvarī;",
  "cnd1:44.3_0": "Tīṇissa lakkhaṇā gatte,",
  "cnd1:44.4_0": "tiṇṇaṁ vedāna pāragū.",
  "cnd1:45.1_0": "Lakkhaṇe itihāse ca,",
  "cnd1:45.2_0": "sanighaṇḍusakeṭubhe;",
  "cnd1:45.3_0": "Pañcasatāni vāceti,",
  "cnd1:45.4_0": "sadhamme pāramiṁ gato”.",
  "cnd1:46.1_0": "“Lakkhaṇānaṁ pavicayaṁ,",
  "cnd1:46.2_0": "bāvarissa naruttama;",
  "cnd1:46.3_0": "Taṇhacchida pakāsehi,",
  "cnd1:46.4_0": "mā no kaṅkhāyitaṁ ahu”.",
  "cnd1:47.1_0": "“Mukhaṁ jivhāya chādeti,",
  "cnd1:47.2_0": "uṇṇassa bhamukantare;",
  "cnd1:47.3_0": "Kosohitaṁ vatthaguyhaṁ,",
  "cnd1:47.4_0": "evaṁ jānāhi māṇava”.",
  "cnd1:48.1_0": "Pucchañhi kiñci asuṇanto,",
  "cnd1:48.2_0": "sutvā pañhe viyākate;",
  "cnd1:48.3_0": "Vicinteti jano sabbo,",
  "cnd1:48.4_0": "vedajāto katañjalī.",
  "cnd1:49.1_0": "“Ko nu devo vā brahmā vā,",
  "cnd1:49.2_0": "indo vāpi sujampati;",
  "cnd1:49.3_0": "Manasā pucchite pañhe,",
  "cnd1:49.4_0": "kametaṁ paṭibhāsati”.",
  "cnd1:50.1_0": "“Muddhaṁ muddhādhipātañca,",
  "cnd1:50.2_0": "bāvarī paripucchati;",
  "cnd1:50.3_0": "Taṁ byākarohi bhagavā,",
  "cnd1:50.4_0": "kaṅkhaṁ vinaya no ise”.",
  "cnd1:51.1_0": "“Avijjā muddhāti jānāhi,",
  "cnd1:51.2_0": "vijjā muddhādhipātinī;",
  "cnd1:51.3_0": "Saddhāsatisamādhīhi,",
  "cnd1:51.4_0": "chandavīriyena saṁyutā”.",
  "cnd1:52.1_0": "Tato vedena mahatā,",
  "cnd1:52.2_0": "santhambhetvāna māṇavo;",
  "cnd1:52.3_0": "Ekaṁsaṁ ajinaṁ katvā,",
  "cnd1:52.4_0": "pādesu sirasā pati.",
  "cnd1:53.1_0": "“Bāvarī brāhmaṇo bhoto,",
  "cnd1:53.2_0": "saha sissehi mārisa;",
  "cnd1:53.3_0": "Udaggacitto sumano,",
  "cnd1:53.4_0": "pāde vandati cakkhuma”.",
  "cnd1:54.1_0": "“Sukhito bāvarī hotu,",
  "cnd1:54.2_0": "saha sissehi brāhmaṇo;",
  "cnd1:54.3_0": "Tvañcāpi sukhito hohi,",
  "cnd1:54.4_0": "ciraṁ jīvāhi māṇava.",
  "cnd1:55.1_0": "Bāvarissa ca tuyhaṁ vā,",
  "cnd1:55.2_0": "sabbesaṁ sabbasaṁsayaṁ;",
  "cnd1:55.3_0": "Katāvakāsā pucchavho,",
  "cnd1:55.4_0": "yaṁ kiñci manasicchatha”.",
  "cnd1:56.1_0": "Sambuddhena katokāso,",
  "cnd1:56.2_0": "nisīditvāna pañjalī;",
  "cnd1:56.3_0": "Ajito paṭhamaṁ pañhaṁ,",
  "cnd1:56.4_0": "tattha pucchi tathāgataṁ.",
  "cnd1:56.5_0": "Vatthugāthā niṭṭhitā."
}