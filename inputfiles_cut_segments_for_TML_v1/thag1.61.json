{
  "thag1.61:0.1_0": "Theragāthā",
  "thag1.61:0.2_0": "Ekakanipāta",
  "thag1.61:0.3_0": "Sattamavagga",
  "thag1.61:0.4_0": "1.61. Vappattheragāthā",
  "thag1.61:1.1_0": "“Passati passo passantaṁ,",
  "thag1.61:1.2_0": "apassantañca passati;",
  "thag1.61:1.3_0": "Apassanto apassantaṁ,",
  "thag1.61:1.4_0": "passantañca na passatī”ti.",
  "thag1.61:1.5_0": "… Vappo thero …."
}