{
  "vv85:0.1_0": "Vimānavatthu",
  "vv85:0.2_0": "Purisavimāna",
  "vv85:0.3_0": "Sunikkhittavagga",
  "vv85:0.4_0": "11. Sunikkhittavimānavatthu",
  "vv85:1.1_0": "“Uccamidaṁ maṇithūṇaṁ vimānaṁ,",
  "vv85:1.2_0": "Samantato dvādasa yojanāni;",
  "vv85:1.3_0": "Kūṭāgārā sattasatā uḷārā,",
  "vv85:1.4_0": "Veḷuriyathambhā rucakatthatā subhā.",
  "vv85:2.1_0": "Tatthacchasi pivasi khādasi ca,",
  "vv85:2.2_0": "Dibbā ca vīṇā pavadanti vagguṁ;",
  "vv85:2.3_0": "Dibbā rasā kāmaguṇettha pañca,",
  "vv85:2.4_0": "Nāriyo ca naccanti suvaṇṇachannā.",
  "vv85:3.1_0": "Kena tetādiso vaṇṇo,",
  "vv85:3.2_0": "kena te idha mijjhati;",
  "vv85:3.3_0": "Uppajjanti ca te bhogā,",
  "vv85:3.4_0": "ye keci manaso piyā.",
  "vv85:4.1_0": "Pucchāmi taṁ deva mahānubhāva,",
  "vv85:4.2_0": "Manussabhūto kimakāsi puññaṁ;",
  "vv85:4.3_0": "Kenāsi evaṁ jalitānubhāvo,",
  "vv85:4.4_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv85:5.1_0": "So devaputto attamano,",
  "vv85:5.2_0": "moggallānena pucchito;",
  "vv85:5.3_0": "Pañhaṁ puṭṭho viyākāsi,",
  "vv85:5.4_0": "yassa kammassidaṁ phalaṁ.",
  "vv85:6.1_0": "“Dunnikkhittaṁ mālaṁ sunikkhipitvā,",
  "vv85:6.2_0": "Patiṭṭhapetvā sugatassa thūpe;",
  "vv85:6.3_0": "Mahiddhiko camhi mahānubhāvo,",
  "vv85:6.4_0": "Dibbehi kāmehi samaṅgibhūto.",
  "vv85:7.1_0": "Tena metādiso vaṇṇo,",
  "vv85:7.2_0": "tena me idha mijjhati;",
  "vv85:7.3_0": "Uppajjanti ca me bhogā,",
  "vv85:7.4_0": "ye keci manaso piyā.",
  "vv85:8.1_0": "Akkhāmi te bhikkhu mahānubhāva,",
  "vv85:8.2_0": "Manussabhūto yamahaṁ akāsiṁ;",
  "vv85:8.3_0": "Tenamhi evaṁ jalitānubhāvo,",
  "vv85:8.4_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv85:9.1_0": "Sunikkhittavimānaṁ ekādasamaṁ.",
  "vv85:9.2_0": "Sunikkhittavaggo sattamo.",
  "vv85:10.1_0": "Tassuddānaṁ",
  "vv85:11.1_0": "Dve daliddā vanavihārā,",
  "vv85:11.2_0": "Bhatako gopālakaṇḍako;",
  "vv85:11.3_0": "Anekavaṇṇamaṭṭhakuṇḍalī,",
  "vv85:11.4_0": "Serīsako sunikkhittaṁ;",
  "vv85:11.5_0": "Purisānaṁ tatiyo vaggo pavuccatīti.",
  "vv85:12.1_0": "Bhāṇavāraṁ catutthaṁ.",
  "vv85:13.1_0": "Vimānavatthupāḷi niṭṭhitā."
}