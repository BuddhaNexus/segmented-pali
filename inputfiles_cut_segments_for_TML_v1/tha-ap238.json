{
  "tha-ap238:0.1_0": "Therāpadāna",
  "tha-ap238:0.2_0": "Udakāsanavagga",
  "tha-ap238:0.3_0": "6 Vaṇṇakārattheraapadāna",
  "tha-ap238:1.1_0": "“Nagare aruṇavatiyā,",
  "tha-ap238:1.2_0": "vaṇṇakāro ahaṁ tadā;",
  "tha-ap238:1.3_0": "Cetiye dussabhaṇḍāni,",
  "tha-ap238:1.4_0": "nānāvaṇṇaṁ rajesahaṁ.",
  "tha-ap238:2.1_0": "Ekattiṁse ito kappe,",
  "tha-ap238:2.2_0": "yaṁ vaṇṇaṁ rajayiṁ tadā;",
  "tha-ap238:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap238:2.4_0": "vaṇṇadānassidaṁ phalaṁ.",
  "tha-ap238:3.1_0": "Ito tevīsatikappe,",
  "tha-ap238:3.2_0": "vaṇṇasamasanāmako;",
  "tha-ap238:3.3_0": "Sattaratanasampanno,",
  "tha-ap238:3.4_0": "cakkavattī mahabbalo.",
  "tha-ap238:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap238:4.2_0": "…pe…",
  "tha-ap238:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap238:5.1_0": "Itthaṁ sudaṁ āyasmā vaṇṇakāro thero imā gāthāyo abhāsitthāti.",
  "tha-ap238:6.1_0": "Vaṇṇakārattherassāpadānaṁ chaṭṭhaṁ."
}