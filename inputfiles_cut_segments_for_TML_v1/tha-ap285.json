{
  "tha-ap285:0.1_0": "Therāpadāna",
  "tha-ap285:0.2_0": "Paṇṇadāyakavagga",
  "tha-ap285:0.3_0": "3. Paccuggamaniyattheraapadāna",
  "tha-ap285:1.1_0": "“Sīhaṁ yathā vanacaraṁ,",
  "tha-ap285:1.2_0": "nisabhājāniyaṁ yathā;",
  "tha-ap285:1.3_0": "Kakudhaṁ vilasantaṁva,",
  "tha-ap285:1.4_0": "āgacchantaṁ narāsabhaṁ.",
  "tha-ap285:2.1_0": "Siddhatthaṁ lokapajjotaṁ,",
  "tha-ap285:2.2_0": "sabbalokatikicchakaṁ;",
  "tha-ap285:2.3_0": "Akāsiṁ paccuggamanaṁ,",
  "tha-ap285:2.4_0": "vippasannena cetasā.",
  "tha-ap285:3.1_0": "Catunnavutito kappe,",
  "tha-ap285:3.2_0": "paccuggacchiṁ narāsabhaṁ;",
  "tha-ap285:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap285:3.4_0": "paccuggamane idaṁ phalaṁ.",
  "tha-ap285:4.1_0": "Sattatiṁse ito kappe,",
  "tha-ap285:4.2_0": "eko āsiṁ janādhipo;",
  "tha-ap285:4.3_0": "Saparivāroti nāmena,",
  "tha-ap285:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap285:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap285:5.2_0": "…pe…",
  "tha-ap285:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap285:6.1_0": "Itthaṁ sudaṁ āyasmā paccuggamaniyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap285:7.1_0": "Paccuggamaniyattherassāpadānaṁ tatiyaṁ."
}