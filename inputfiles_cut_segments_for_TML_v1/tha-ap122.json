{
  "tha-ap122:0.1_0": "Therāpadāna",
  "tha-ap122:0.2_0": "Mahāparivāravagga",
  "tha-ap122:0.3_0": "10. Jātipūjakattheraapadāna",
  "tha-ap122:1.1_0": "“Jāyantassa vipassissa,",
  "tha-ap122:1.2_0": "āloko vipulo ahu;",
  "tha-ap122:1.3_0": "Pathavī ca pakampittha,",
  "tha-ap122:1.4_0": "sasāgarā sapabbatā.",
  "tha-ap122:2.1_0": "Nemittā ca viyākaṁsu,",
  "tha-ap122:2.2_0": "buddho loke bhavissati;",
  "tha-ap122:2.3_0": "Aggo ca sabbasattānaṁ,",
  "tha-ap122:2.4_0": "janataṁ uddharissati.",
  "tha-ap122:3.1_0": "Nemittānaṁ suṇitvāna,",
  "tha-ap122:3.2_0": "jātipūjamakāsahaṁ;",
  "tha-ap122:3.3_0": "Edisā pūjanā natthi,",
  "tha-ap122:3.4_0": "yādisā jātipūjanā.",
  "tha-ap122:4.1_0": "Saṅkharitvāna kusalaṁ,",
  "tha-ap122:4.2_0": "sakaṁ cittaṁ pasādayiṁ;",
  "tha-ap122:4.3_0": "Jātipūjaṁ karitvāna,",
  "tha-ap122:4.4_0": "tattha kālaṅkato ahaṁ.",
  "tha-ap122:5.1_0": "Yaṁ yaṁ yonupapajjāmi,",
  "tha-ap122:5.2_0": "devattaṁ atha mānusaṁ;",
  "tha-ap122:5.3_0": "Sabbe satte abhibhomi,",
  "tha-ap122:5.4_0": "jātipūjāyidaṁ phalaṁ.",
  "tha-ap122:6.1_0": "Dhātiyo maṁ upaṭṭhanti,",
  "tha-ap122:6.2_0": "mama cittavasānugā;",
  "tha-ap122:6.3_0": "Na tā sakkonti kopetuṁ,",
  "tha-ap122:6.4_0": "jātipūjāyidaṁ phalaṁ.",
  "tha-ap122:7.1_0": "Ekanavutito kappe,",
  "tha-ap122:7.2_0": "yaṁ pūjamakariṁ tadā;",
  "tha-ap122:7.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap122:7.4_0": "jātipūjāyidaṁ phalaṁ.",
  "tha-ap122:8.1_0": "Supāricariyā nāma,",
  "tha-ap122:8.2_0": "catuttiṁsa janādhipā;",
  "tha-ap122:8.3_0": "Ito tatiyakappamhi,",
  "tha-ap122:8.4_0": "cakkavattī mahabbalā.",
  "tha-ap122:9.1_0": "Paṭisambhidā catasso,",
  "tha-ap122:9.2_0": "…pe…",
  "tha-ap122:9.3_0": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap122:10.1_0": "Itthaṁ sudaṁ āyasmā jātipūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap122:11.1_0": "Jātipūjakattherassāpadānaṁ dasamaṁ.",
  "tha-ap122:12.1_0": "Mahāparivāravaggo dvādasamo.",
  "tha-ap122:13.1_0": "Tassuddānaṁ",
  "tha-ap122:14.1_0": "Parivārasumaṅgalā,",
  "tha-ap122:14.2_0": "saraṇāsanapupphiyā;",
  "tha-ap122:14.3_0": "Citapūjī buddhasaññī,",
  "tha-ap122:14.4_0": "maggupaṭṭhānajātinā;",
  "tha-ap122:14.5_0": "Gāthāyo navuti vuttā,",
  "tha-ap122:14.6_0": "gaṇitāyo vibhāvihi."
}