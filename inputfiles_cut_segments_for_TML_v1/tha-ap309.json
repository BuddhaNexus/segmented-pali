{
  "tha-ap309:0.1_0": "Therāpadāna",
  "tha-ap309:0.2_0": "Padumakesaravagga",
  "tha-ap309:0.3_0": "7. Ārāmadāyakattheraapadāna",
  "tha-ap309:1.1_0": "“Siddhatthassa bhagavato,",
  "tha-ap309:1.2_0": "ārāmo ropito mayā;",
  "tha-ap309:1.3_0": "Sandacchāyesu rukkhesu,",
  "tha-ap309:1.4_0": "upāsantesu pakkhisu.",
  "tha-ap309:2.1_0": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap309:2.2_0": "āhutīnaṁ paṭiggahaṁ;",
  "tha-ap309:2.3_0": "Ārāmaṁ abhināmesiṁ,",
  "tha-ap309:2.4_0": "lokajeṭṭhaṁ narāsabhaṁ.",
  "tha-ap309:3.1_0": "Haṭṭho haṭṭhena cittena,",
  "tha-ap309:3.2_0": "phalaṁ pupphamadāsahaṁ;",
  "tha-ap309:3.3_0": "Tato jātappasādova,",
  "tha-ap309:3.4_0": "taṁ vanaṁ pariṇāmayiṁ.",
  "tha-ap309:4.1_0": "Buddhassa yamidaṁ dāsiṁ,",
  "tha-ap309:4.2_0": "vippasannena cetasā;",
  "tha-ap309:4.3_0": "Bhave nibbattamānamhi,",
  "tha-ap309:4.4_0": "nibbattati phalaṁ mama.",
  "tha-ap309:5.1_0": "Catunnavutito kappe,",
  "tha-ap309:5.2_0": "yaṁ ārāmamadaṁ tadā;",
  "tha-ap309:5.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap309:5.4_0": "ārāmassa idaṁ phalaṁ.",
  "tha-ap309:6.1_0": "Sattatiṁse ito kappe,",
  "tha-ap309:6.2_0": "sattāsuṁ mudusītalā;",
  "tha-ap309:6.3_0": "Sattaratanasampannā,",
  "tha-ap309:6.4_0": "cakkavattī mahabbalā.",
  "tha-ap309:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap309:7.2_0": "…pe…",
  "tha-ap309:7.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap309:8.1_0": "Itthaṁ sudaṁ āyasmā ārāmadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap309:9.1_0": "Ārāmadāyakattherassāpadānaṁ sattamaṁ."
}