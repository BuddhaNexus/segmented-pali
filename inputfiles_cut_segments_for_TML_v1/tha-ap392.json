{
  "tha-ap392:0.1_0": "Therāpadāna",
  "tha-ap392:0.2_0": "Avaṭaphalavagga",
  "tha-ap392:0.3_0": "10 Pubbakammapilotikabuddhaapadāna",
  "tha-ap392:1.1_0": "Anotattasarāsanne,",
  "tha-ap392:1.2_0": "ramaṇīye silātale;",
  "tha-ap392:1.3_0": "Nānāratanapajjote,",
  "tha-ap392:1.4_0": "nānāgandhavanantare.",
  "tha-ap392:2.1_0": "Mahatā bhikkhusaṅghena,",
  "tha-ap392:2.2_0": "pareto lokanāyako;",
  "tha-ap392:2.3_0": "Āsīno byākarī tattha,",
  "tha-ap392:2.4_0": "pubbakammāni attano.",
  "tha-ap392:3.1_0": "“Suṇātha bhikkhavo mayhaṁ,",
  "tha-ap392:3.2_0": "yaṁ kammaṁ pakataṁ mayā;",
  "tha-ap392:3.3_0": "Pilotikassa kammassa,",
  "tha-ap392:3.4_0": "buddhattepi vipaccati.",
  "tha-ap392:4.0_0": "10.1",
  "tha-ap392:4.1_0": "Munāḷi nāmahaṁ dhutto,",
  "tha-ap392:4.2_0": "pubbe aññāsu jātisu;",
  "tha-ap392:4.3_0": "Paccekabuddhaṁ surabhiṁ,",
  "tha-ap392:4.4_0": "abbhācikkhiṁ adūsakaṁ.",
  "tha-ap392:5.1_0": "Tena kammavipākena,",
  "tha-ap392:5.2_0": "niraye saṁsariṁ ciraṁ;",
  "tha-ap392:5.3_0": "Bahūvassasahassāni,",
  "tha-ap392:5.4_0": "dukkhaṁ vedesi vedanaṁ.",
  "tha-ap392:6.1_0": "Tena kammāvasesena,",
  "tha-ap392:6.2_0": "idha pacchimake bhave;",
  "tha-ap392:6.3_0": "Abbhakkhānaṁ mayā laddhaṁ,",
  "tha-ap392:6.4_0": "sundarikāya kāraṇā.",
  "tha-ap392:7.0_0": "10.2",
  "tha-ap392:7.1_0": "Sabbābhibhussa buddhassa,",
  "tha-ap392:7.2_0": "nando nāmāsi sāvako;",
  "tha-ap392:7.3_0": "Taṁ abbhakkhāya niraye,",
  "tha-ap392:7.4_0": "ciraṁ saṁsaritaṁ mayā.",
  "tha-ap392:8.1_0": "Dasavassasahassāni,",
  "tha-ap392:8.2_0": "niraye saṁsariṁ ciraṁ;",
  "tha-ap392:8.3_0": "Manussabhāvaṁ laddhāhaṁ,",
  "tha-ap392:8.4_0": "abbhakkhānaṁ bahuṁ labhiṁ.",
  "tha-ap392:9.1_0": "Tena kammāvasesena,",
  "tha-ap392:9.2_0": "ciñcamānavikā mamaṁ;",
  "tha-ap392:9.3_0": "Abbhācikkhi abhūtena,",
  "tha-ap392:9.4_0": "janakāyassa aggato.",
  "tha-ap392:10.0_0": "10.3",
  "tha-ap392:10.1_0": "Brāhmaṇo sutavā āsiṁ,",
  "tha-ap392:10.2_0": "ahaṁ sakkatapūjito;",
  "tha-ap392:10.3_0": "Mahāvane pañcasate,",
  "tha-ap392:10.4_0": "mante vācemi māṇave.",
  "tha-ap392:11.1_0": "Tatthāgato isi bhīmo,",
  "tha-ap392:11.2_0": "pañcābhiñño mahiddhiko;",
  "tha-ap392:11.3_0": "Tañcāhaṁ āgataṁ disvā,",
  "tha-ap392:11.4_0": "abbhācikkhiṁ adūsakaṁ.",
  "tha-ap392:12.1_0": "Tatohaṁ avacaṁ sisse,",
  "tha-ap392:12.2_0": "kāmabhogī ayaṁ isi;",
  "tha-ap392:12.3_0": "Mayhampi bhāsamānassa,",
  "tha-ap392:12.4_0": "anumodiṁsu māṇavā.",
  "tha-ap392:13.1_0": "Tato māṇavakā sabbe,",
  "tha-ap392:13.2_0": "bhikkhamānaṁ kule kule;",
  "tha-ap392:13.3_0": "Mahājanassa āhaṁsu,",
  "tha-ap392:13.4_0": "kāmabhogī ayaṁ isi.",
  "tha-ap392:14.1_0": "Tena kammavipākena,",
  "tha-ap392:14.2_0": "pañca bhikkhusatā ime;",
  "tha-ap392:14.3_0": "Abbhakkhānaṁ labhuṁ sabbe,",
  "tha-ap392:14.4_0": "sundarikāya kāraṇā.",
  "tha-ap392:15.0_0": "10.4",
  "tha-ap392:15.1_0": "Vemātubhātaraṁ pubbe,",
  "tha-ap392:15.2_0": "dhanahetu haniṁ ahaṁ;",
  "tha-ap392:15.3_0": "Pakkhipiṁ giriduggasmiṁ,",
  "tha-ap392:15.4_0": "silāya ca apiṁsayiṁ.",
  "tha-ap392:16.1_0": "Tena kammavipākena,",
  "tha-ap392:16.2_0": "devadatto silaṁ khipi;",
  "tha-ap392:16.3_0": "Aṅguṭṭhaṁ piṁsayī pāde,",
  "tha-ap392:16.4_0": "mama pāsāṇasakkharā.",
  "tha-ap392:17.0_0": "10.5",
  "tha-ap392:17.1_0": "Purehaṁ dārako hutvā,",
  "tha-ap392:17.2_0": "kīḷamāno mahāpathe;",
  "tha-ap392:17.3_0": "Paccekabuddhaṁ disvāna,",
  "tha-ap392:17.4_0": "magge sakalikaṁ khipiṁ.",
  "tha-ap392:18.1_0": "Tena kammavipākena,",
  "tha-ap392:18.2_0": "idha pacchimake bhave;",
  "tha-ap392:18.3_0": "Vadhatthaṁ maṁ devadatto,",
  "tha-ap392:18.4_0": "abhimāre payojayi.",
  "tha-ap392:19.0_0": "10.6",
  "tha-ap392:19.1_0": "Hatthāroho pure āsiṁ,",
  "tha-ap392:19.2_0": "paccekamunimuttamaṁ;",
  "tha-ap392:19.3_0": "Piṇḍāya vicarantaṁ taṁ,",
  "tha-ap392:19.4_0": "āsādesiṁ gajenahaṁ.",
  "tha-ap392:20.1_0": "Tena kammavipākena,",
  "tha-ap392:20.2_0": "bhanto nāḷāgirī gajo;",
  "tha-ap392:20.3_0": "Giribbaje puravare,",
  "tha-ap392:20.4_0": "dāruṇo samupāgami.",
  "tha-ap392:21.0_0": "10.7",
  "tha-ap392:21.1_0": "Rājāhaṁ patthivo āsiṁ,",
  "tha-ap392:21.2_0": "sattiyā purisaṁ haniṁ;",
  "tha-ap392:21.3_0": "Tena kammavipākena,",
  "tha-ap392:21.4_0": "niraye paccisaṁ bhusaṁ.",
  "tha-ap392:22.1_0": "Kammuno tassa sesena,",
  "tha-ap392:22.2_0": "idāni sakalaṁ mama;",
  "tha-ap392:22.3_0": "Pāde chaviṁ pakappesi,",
  "tha-ap392:22.4_0": "na hi kammaṁ vinassati.",
  "tha-ap392:23.0_0": "10.8",
  "tha-ap392:23.1_0": "Ahaṁ kevaṭṭagāmasmiṁ,",
  "tha-ap392:23.2_0": "ahuṁ kevaṭṭadārako;",
  "tha-ap392:23.3_0": "Macchake ghātite disvā,",
  "tha-ap392:23.4_0": "janayiṁ somanassakaṁ.",
  "tha-ap392:24.1_0": "Tena kammavipākena,",
  "tha-ap392:24.2_0": "sīsadukkhaṁ ahū mama;",
  "tha-ap392:24.3_0": "Sabbe sakkā ca haññiṁsu,",
  "tha-ap392:24.4_0": "yadā hani viṭaṭūbho.",
  "tha-ap392:25.0_0": "10.9",
  "tha-ap392:25.1_0": "Phussassāhaṁ pāvacane,",
  "tha-ap392:25.2_0": "sāvake paribhāsayiṁ;",
  "tha-ap392:25.3_0": "Yavaṁ khādatha bhuñjatha,",
  "tha-ap392:25.4_0": "mā ca bhuñjatha sālayo.",
  "tha-ap392:26.1_0": "Tena kammavipākena,",
  "tha-ap392:26.2_0": "temāsaṁ khāditaṁ yavaṁ;",
  "tha-ap392:26.3_0": "Nimantito brāhmaṇena,",
  "tha-ap392:26.4_0": "verañjāyaṁ vasiṁ tadā.",
  "tha-ap392:27.0_0": "10.10",
  "tha-ap392:27.1_0": "Nibbuddhe vattamānamhi,",
  "tha-ap392:27.2_0": "mallaputtaṁ niheṭhayiṁ;",
  "tha-ap392:27.3_0": "Tena kammavipākena,",
  "tha-ap392:27.4_0": "piṭṭhidukkhaṁ ahū mama.",
  "tha-ap392:28.0_0": "10.11",
  "tha-ap392:28.1_0": "Tikicchako ahaṁ āsiṁ,",
  "tha-ap392:28.2_0": "seṭṭhiputtaṁ virecayiṁ;",
  "tha-ap392:28.3_0": "Tena kammavipākena,",
  "tha-ap392:28.4_0": "hoti pakkhandikā mama.",
  "tha-ap392:29.0_0": "10.12",
  "tha-ap392:29.1_0": "Avacāhaṁ jotipālo,",
  "tha-ap392:29.2_0": "sugataṁ kassapaṁ tadā;",
  "tha-ap392:29.3_0": "Kuto nu bodhi muṇḍassa,",
  "tha-ap392:29.4_0": "bodhi paramadullabhā.",
  "tha-ap392:30.1_0": "Tena kammavipākena,",
  "tha-ap392:30.2_0": "acariṁ dukkaraṁ bahuṁ;",
  "tha-ap392:30.3_0": "Chabbassānuruvelāyaṁ,",
  "tha-ap392:30.4_0": "tato bodhimapāpuṇiṁ.",
  "tha-ap392:31.1_0": "Nāhaṁ etena maggena,",
  "tha-ap392:31.2_0": "pāpuṇiṁ bodhimuttamaṁ;",
  "tha-ap392:31.3_0": "Kummaggena gavesissaṁ,",
  "tha-ap392:31.4_0": "pubbakammena vārito.",
  "tha-ap392:32.1_0": "Puññapāpaparikkhīṇo,",
  "tha-ap392:32.2_0": "sabbasantāpavajjito;",
  "tha-ap392:32.3_0": "Asoko anupāyāso,",
  "tha-ap392:32.4_0": "nibbāyissamanāsavo”.",
  "tha-ap392:33.1_0": "Evaṁ jino viyākāsi,",
  "tha-ap392:33.2_0": "Bhikkhusaṅghassa aggato;",
  "tha-ap392:33.3_0": "Sabbābhiññābalappatto,",
  "tha-ap392:33.4_0": "Anotatte mahāsareti. ",
  "tha-ap392:34.1_0": "Itthaṁ sudaṁ bhagavā attano pubbacaritaṁ kammapilotikaṁ nāma buddhāpadānadhammapariyāyaṁ abhāsitthāti.",
  "tha-ap392:35.1_0": "Pubbakammapilotikaṁ nāma buddhāpadānaṁ dasamaṁ.",
  "tha-ap392:36.1_0": "Avaṭaphalavaggo ekūnacattālīsamo.",
  "tha-ap392:37.1_0": "Tassuddānaṁ",
  "tha-ap392:38.1_0": "Avaṭaṁ labujañceva,",
  "tha-ap392:38.2_0": "udumbarapilakkhu ca;",
  "tha-ap392:38.3_0": "Phāru vallī ca kadalī,",
  "tha-ap392:38.4_0": "panaso koṭivīsako.",
  "tha-ap392:39.1_0": "Pubbakammapiloti ca,",
  "tha-ap392:39.2_0": "apadānaṁ mahesino;",
  "tha-ap392:39.3_0": "Gāthāyo ekanavuti,",
  "tha-ap392:39.4_0": "gaṇitāyo vibhāvibhi.",
  "tha-ap392:40.1_0": "Cuddasamaṁ bhāṇavāraṁ."
}