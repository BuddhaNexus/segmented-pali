{
  "tika-vin10t0:0": " Namo tassa bhagavato arahato sammāsambuddhassa",
  "tika-vin10t0:1": "Vinayavinicchayo",
  "tika-vin10t0:2": "Ganthārambhakathā",
  "tika-vin10t0:3": "1.",
  "tika-vin10t0:4": " Vanditvā sirasā seṭṭhaṃ, buddhamappaṭipuggalaṃ;",
  "tika-vin10t0:5": "Bhavābhāvakaraṃ dhammaṃ, gaṇañceva niraṅgaṇaṃ.",
  "tika-vin10t0:6": "2.",
  "tika-vin10t0:7": " Bhikkhūnaṃ bhikkhunīnañca, hitatthāya samāhito;",
  "tika-vin10t0:8": "Pavakkhāmi samāsena, vinayassavinicchayaṃ.",
  "tika-vin10t0:9": "3.",
  "tika-vin10t0:10": " Anākulamasaṃkiṇṇaṃ, madhuratthapadakkamaṃ;",
  "tika-vin10t0:11": "Paṭubhāvakaraṃ etaṃ, paramaṃ vinayakkame.",
  "tika-vin10t0:12": "4.",
  "tika-vin10t0:13": " Apāraṃ otarantānaṃ, sāraṃ vinayasāgaraṃ;",
  "tika-vin10t0:14": "Bhikkhūnaṃ bhikkhunīnañca, nāvābhūtaṃ manoramaṃ.",
  "tika-vin10t0:15": "5.",
  "tika-vin10t0:16": " Tasmā vinayanūpāyaṃ, vinayassavinicchayaṃ;",
  "tika-vin10t0:17": "Avikkhittena cittena, vadato me nibodhatha."
}