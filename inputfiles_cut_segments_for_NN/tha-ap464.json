{
  "tha-ap464:0.1_0": "Therāpadāna",
  "tha-ap464:0.2_0": "Sālakusumiyavagga",
  "tha-ap464:0.3_0": "2. Citakapūjakattheraapadāna",
  "tha-ap464:1.1_0": "“Jhāyamānassa bhagavato,",
  "tha-ap464:1.2_0": "sikhino lokabandhuno;",
  "tha-ap464:1.3_0": "Aṭṭha campakapupphāni,",
  "tha-ap464:1.4_0": "citakaṁ abhiropayiṁ.",
  "tha-ap464:2.1_0": "Ekatiṁse ito kappe,",
  "tha-ap464:2.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap464:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap464:2.4_0": "citapūjāyidaṁ phalaṁ.",
  "tha-ap464:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap464:3.2_0": "…pe…",
  "tha-ap464:3.3_0": "viharāmi anāsavo.",
  "tha-ap464:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap464:4.2_0": "…pe…",
  "tha-ap464:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap464:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap464:5.2_0": "…pe…",
  "tha-ap464:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap464:6.1_0": "Itthaṁ sudaṁ āyasmā citakapūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap464:7.1_0": "Citakapūjakattherassāpadānaṁ dutiyaṁ."
}