{
  "tha-ap175:0.1_0": "Therāpadāna",
  "tha-ap175:0.2_0": "Kumudavagga",
  "tha-ap175:0.3_0": "3 Rattipupphiyattheraapadāna",
  "tha-ap175:1.1_0": "“Migaluddo pure āsiṁ,",
  "tha-ap175:1.2_0": "araññe kānane ahaṁ;",
  "tha-ap175:1.3_0": "Vipassiṁ addasaṁ buddhaṁ,",
  "tha-ap175:1.4_0": "devadevaṁ narāsabhaṁ.",
  "tha-ap175:2.1_0": "Rattikaṁ pupphitaṁ disvā,",
  "tha-ap175:2.2_0": "kuṭajaṁ dharaṇīruhaṁ;",
  "tha-ap175:2.3_0": "Samūlaṁ paggahetvāna,",
  "tha-ap175:2.4_0": "upanesiṁ mahesino.",
  "tha-ap175:3.1_0": "Ekanavutito kappe,",
  "tha-ap175:3.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap175:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap175:3.4_0": "pupphadānassidaṁ phalaṁ.",
  "tha-ap175:4.1_0": "Ito ca aṭṭhame kappe,",
  "tha-ap175:4.2_0": "suppasannasanāmako;",
  "tha-ap175:4.3_0": "Sattaratanasampanno,",
  "tha-ap175:4.4_0": "rājāhosiṁ mahabbalo.",
  "tha-ap175:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap175:5.2_0": "…pe…",
  "tha-ap175:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap175:6.1_0": "Itthaṁ sudaṁ āyasmā rattipupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap175:7.1_0": "Rattipupphiyattherassāpadānaṁ tatiyaṁ."
}