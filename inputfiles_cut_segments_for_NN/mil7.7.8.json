{
  "mil7.7.8:0.1_0": "Milindapañha",
  "mil7.7.8:0.2_0": "Opammakathāpañha",
  "mil7.7.8:0.3_0": "Makkaṭakavagga",
  "mil7.7.8:0.4_0": "8. Māgavikaṅgapañha",
  "mil7.7.8:1.1_0": "“Bhante nāgasena,",
  "mil7.7.8:1.1_1": "‘māgavikassa cattāri aṅgāni gahetabbānī’ti yaṁ vadesi,",
  "mil7.7.8:1.1_2": "katamāni tāni cattāri aṅgāni gahetabbānī”ti?",
  "mil7.7.8:1.2_0": "“Yathā, mahārāja, māgaviko appamiddho hoti;",
  "mil7.7.8:1.3_0": "evameva kho, mahārāja,",
  "mil7.7.8:1.3_1": "yoginā yogāvacarena appamiddhena bhavitabbaṁ.",
  "mil7.7.8:1.4_0": "Idaṁ, mahārāja,",
  "mil7.7.8:1.4_1": "māgavikassa paṭhamaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.7.8:2.1_0": "Puna caparaṁ, mahārāja,",
  "mil7.7.8:2.1_1": "māgaviko migesuyeva cittaṁ upanibandhati;",
  "mil7.7.8:2.2_0": "evameva kho, mahārāja,",
  "mil7.7.8:2.2_1": "yoginā yogāvacarena ārammaṇesuyeva cittaṁ upanibandhitabbaṁ.",
  "mil7.7.8:2.3_0": "Idaṁ, mahārāja,",
  "mil7.7.8:2.3_1": "māgavikassa dutiyaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.7.8:3.1_0": "Puna caparaṁ, mahārāja,",
  "mil7.7.8:3.1_1": "māgaviko kālaṁ kammassa jānāti;",
  "mil7.7.8:3.2_0": "evameva kho, mahārāja,",
  "mil7.7.8:3.2_1": "yoginā yogāvacarena paṭisallānassa kālo jānitabbo ‘ayaṁ kālo paṭisallānassa, ayaṁ kālo nikkhamanāyā’ti.",
  "mil7.7.8:3.3_0": "Idaṁ, mahārāja,",
  "mil7.7.8:3.3_1": "māgavikassa tatiyaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.7.8:4.1_0": "Puna caparaṁ, mahārāja,",
  "mil7.7.8:4.1_1": "māgaviko migaṁ disvā hāsamabhijaneti ‘imaṁ lacchāmī’ti;",
  "mil7.7.8:4.2_0": "evameva kho, mahārāja,",
  "mil7.7.8:4.2_1": "yoginā yogāvacarena ārammaṇe abhiramitabbaṁ,",
  "mil7.7.8:4.2_2": "hāsamabhijanetabbaṁ ‘uttariṁ visesamadhigacchissāmī’ti.",
  "mil7.7.8:4.3_0": "Idaṁ, mahārāja,",
  "mil7.7.8:4.3_1": "māgavikassa catutthaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.7.8:4.4_0": "Bhāsitampetaṁ, mahārāja, therena mogharājena—",
  "mil7.7.8:5.1_0": "‘Ārammaṇe labhitvāna,",
  "mil7.7.8:5.2_0": "Pahitattena bhikkhunā;",
  "mil7.7.8:5.3_0": "Bhiyyo hāso janetabbo,",
  "mil7.7.8:5.4_0": "Adhigacchissāmi uttarin’”ti.",
  "mil7.7.8:6.1_0": "Māgavikaṅgapañho aṭṭhamo."
}