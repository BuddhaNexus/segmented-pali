{
  "iti60:0.1_0": "Itivuttaka 60",
  "iti60:0.2_0": "Tikanipāta",
  "iti60:0.3_0": "Dutiyavagga",
  "iti60:0.4_0": "Puññakiriyavatthusutta",
  "iti60:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti60:2.1_0": "“Tīṇimāni, bhikkhave, puññakiriyavatthūni.",
  "iti60:2.2_0": "Katamāni tīṇi?",
  "iti60:2.3_0": "Dānamayaṁ puññakiriyavatthu, sīlamayaṁ puññakiriyavatthu,",
  "iti60:2.3_1": "bhāvanāmayaṁ puññakiriyavatthu—",
  "iti60:2.4_0": "imāni kho, bhikkhave, tīṇi puññakiriyavatthūnī”ti.",
  "iti60:2.5_0": "Etamatthaṁ bhagavā avoca.",
  "iti60:2.6_0": "Tatthetaṁ iti vuccati:",
  "iti60:3.1_0": "“Puññameva so sikkheyya,",
  "iti60:3.2_0": "āyataggaṁ sukhudrayaṁ;",
  "iti60:3.3_0": "Dānañca samacariyañca,",
  "iti60:3.4_0": "mettacittañca bhāvaye.",
  "iti60:4.1_0": "Ete dhamme bhāvayitvā,",
  "iti60:4.2_0": "tayo sukhasamuddaye;",
  "iti60:4.3_0": "Abyābajjhaṁ sukhaṁ lokaṁ,",
  "iti60:4.4_0": "paṇḍito upapajjatī”ti.",
  "iti60:5.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti60:5.2_0": "Paṭhamaṁ."
}