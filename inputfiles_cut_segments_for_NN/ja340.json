{
  "ja340:0.1_0": "Jātaka",
  "ja340:0.2_0": "Catukkanipāta",
  "ja340:0.3_0": "Kokilavagga",
  "ja340:0.4_0": "10. Visayhajātaka",
  "ja340:1.1_0": "“Adāsi dānāni pure visayha,",
  "ja340:1.2_0": "Dadato ca te khayadhammo ahosi;",
  "ja340:1.3_0": "Ito parañce na dadeyya dānaṁ,",
  "ja340:1.4_0": "Tiṭṭheyyuṁ te saṁyamantassa bhogā”.",
  "ja340:2.1_0": "“Anariyamariyena sahassanetta,",
  "ja340:2.2_0": "Suduggatenāpi akiccamāhu;",
  "ja340:2.3_0": "Mā vo dhanaṁ taṁ ahu devarāja,",
  "ja340:2.4_0": "Yaṁ bhogahetu vijahemu saddhaṁ.",
  "ja340:3.1_0": "Yena eko ratho yāti,",
  "ja340:3.2_0": "yāti tenāparo ratho;",
  "ja340:3.3_0": "Porāṇaṁ nihitaṁ vattaṁ,",
  "ja340:3.4_0": "vattataññeva vāsava.",
  "ja340:4.1_0": "Yadi hessati dassāma,",
  "ja340:4.2_0": "asante kiṁ dadāmase;",
  "ja340:4.3_0": "Evaṁbhūtāpi dassāma,",
  "ja340:4.4_0": "mā dānaṁ pamadamhase”ti.",
  "ja340:5.1_0": "Visayhajātakaṁ dasamaṁ.",
  "ja340:5.2_0": "Kokilavaggo catuttho.",
  "ja340:6.1_0": "Tassuddānaṁ",
  "ja340:7.1_0": "Ativelapabhāsati jītavaro,",
  "ja340:7.2_0": "Vanamajjha rathesabha jimhagamo;",
  "ja340:7.3_0": "Atha jambu tiṇāsanapīṭhavaraṁ,",
  "ja340:7.4_0": "Atha taṇḍula mora visayha dasāti."
}