{
  "tha-ap390:0.1_0": "Therāpadāna",
  "tha-ap390:0.2_0": "Avaṭaphalavagga",
  "tha-ap390:0.3_0": "8. Panasaphaladāyakattheraapadāna",
  "tha-ap390:1.1_0": "“Ajjuno nāma sambuddho,",
  "tha-ap390:1.2_0": "himavante vasī tadā;",
  "tha-ap390:1.3_0": "Caraṇena ca sampanno,",
  "tha-ap390:1.4_0": "samādhikusalo muni.",
  "tha-ap390:2.1_0": "Kumbhamattaṁ gahetvāna,",
  "tha-ap390:2.2_0": "panasaṁ jīvajīvakaṁ;",
  "tha-ap390:2.3_0": "Chattapaṇṇe ṭhapetvāna,",
  "tha-ap390:2.4_0": "adāsiṁ satthuno ahaṁ.",
  "tha-ap390:3.1_0": "Ekattiṁse ito kappe,",
  "tha-ap390:3.2_0": "yaṁ phalamadadiṁ tadā;",
  "tha-ap390:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap390:3.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap390:4.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap390:4.2_0": "…pe…",
  "tha-ap390:4.3_0": "viharāmi anāsavo.",
  "tha-ap390:5.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap390:5.2_0": "…pe…",
  "tha-ap390:5.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap390:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap390:6.2_0": "…pe…",
  "tha-ap390:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap390:7.1_0": "Itthaṁ sudaṁ āyasmā panasaphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap390:8.1_0": "Panasaphaladāyakattherassāpadānaṁ aṭṭhamaṁ."
}