{
  "kv11.3:0.1_0": "Kathāvatthu",
  "kv11.3:0.2_0": "Mahāpaṇṇāsaka",
  "kv11.3:0.3_0": "Ekādasamavagga",
  "kv11.3:0.4_0": "Ñāṇaṁcittavippayuttantikathā",
  "kv11.3:1.1_0": "Ñāṇaṁ cittavippayuttanti?",
  "kv11.3:1.2_0": "Āmantā.",
  "kv11.3:1.3_0": "Rūpaṁ nibbānaṁ cakkhāyatanaṁ …pe… phoṭṭhabbāyatananti?",
  "kv11.3:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv11.3:1.5_0": "ñāṇaṁ cittavippayuttanti?",
  "kv11.3:1.6_0": "Āmantā.",
  "kv11.3:1.7_0": "Paññā paññindriyaṁ paññābalaṁ sammādiṭṭhi dhammavicayasambojjhaṅgo cittavippayuttoti?",
  "kv11.3:1.8_0": "Na hevaṁ vattabbe …pe…",
  "kv11.3:1.9_0": "paññā paññindriyaṁ paññābalaṁ sammādiṭṭhi dhammavicayasambojjhaṅgo cittasampayuttoti?",
  "kv11.3:1.10_0": "Āmantā.",
  "kv11.3:1.11_0": "Ñāṇaṁ cittasampayuttanti?",
  "kv11.3:1.12_0": "Na hevaṁ vattabbe …pe….",
  "kv11.3:2.1_0": "Ñāṇaṁ cittavippayuttanti?",
  "kv11.3:2.2_0": "Āmantā.",
  "kv11.3:2.3_0": "Katamakkhandhapariyāpannanti?",
  "kv11.3:2.4_0": "Saṅkhārakkhandhapariyāpannanti.",
  "kv11.3:2.5_0": "Saṅkhārakkhandho cittavippayuttoti?",
  "kv11.3:2.6_0": "Na hevaṁ vattabbe …pe…",
  "kv11.3:2.7_0": "saṅkhārakkhandho cittavippayuttoti?",
  "kv11.3:2.8_0": "Āmantā.",
  "kv11.3:2.9_0": "Vedanākkhandho saññākkhandho cittavippayuttoti?",
  "kv11.3:2.10_0": "Na hevaṁ vattabbe …pe…",
  "kv11.3:2.11_0": "ñāṇaṁ saṅkhārakkhandhapariyāpannaṁ cittavippayuttanti?",
  "kv11.3:2.12_0": "Āmantā.",
  "kv11.3:2.13_0": "Paññā saṅkhārakkhandhapariyāpannā cittavippayuttāti?",
  "kv11.3:2.14_0": "Na hevaṁ vattabbe …pe…",
  "kv11.3:2.15_0": "paññā saṅkhārakkhandhapariyāpannā cittasampayuttāti?",
  "kv11.3:2.16_0": "Āmantā.",
  "kv11.3:2.17_0": "Ñāṇaṁ saṅkhārakkhandhapariyāpannaṁ cittasampayuttanti?",
  "kv11.3:2.18_0": "Na hevaṁ vattabbe …pe…",
  "kv11.3:2.19_0": "ñāṇaṁ saṅkhārakkhandhapariyāpannaṁ cittavippayuttaṁ,",
  "kv11.3:2.19_1": "paññā saṅkhārakkhandhapariyāpannā cittasampayuttāti?",
  "kv11.3:2.20_0": "Āmantā.",
  "kv11.3:2.21_0": "Saṅkhārakkhandho ekadeso cittasampayutto ekadeso cittavippayuttoti?",
  "kv11.3:2.22_0": "Na hevaṁ vattabbe …pe…",
  "kv11.3:2.23_0": "saṅkhārakkhandho ekadeso cittasampayutto ekadeso cittavippayuttoti?",
  "kv11.3:2.24_0": "Āmantā.",
  "kv11.3:2.25_0": "Vedanākkhandho saññākkhandho ekadeso cittasampayutto ekadeso cittavippayuttoti?",
  "kv11.3:2.26_0": "Na hevaṁ vattabbe …pe….",
  "kv11.3:3.1_0": "Na vattabbaṁ—",
  "kv11.3:3.2_0": "“ñāṇaṁ cittavippayuttan”ti?",
  "kv11.3:3.3_0": "Āmantā.",
  "kv11.3:3.4_0": "Arahā cakkhuviññāṇasamaṅgī “ñāṇī”ti vattabboti?",
  "kv11.3:3.5_0": "Āmantā.",
  "kv11.3:3.6_0": "Ñāṇaṁ tena cittena sampayuttanti?",
  "kv11.3:3.7_0": "Na hevaṁ vattabbe.",
  "kv11.3:3.8_0": "Tena hi ñāṇaṁ cittavippayuttanti.",
  "kv11.3:4.1_0": "Arahā cakkhuviññāṇasamaṅgī “paññavā”ti vattabboti?",
  "kv11.3:4.2_0": "Āmantā.",
  "kv11.3:4.3_0": "Paññā tena cittena sampayuttāti?",
  "kv11.3:4.4_0": "Na hevaṁ vattabbe.",
  "kv11.3:4.5_0": "Tena hi paññā cittavippayuttāti.",
  "kv11.3:4.6_0": "Ñāṇaṁ cittavippayuttantikathā niṭṭhitā."
}