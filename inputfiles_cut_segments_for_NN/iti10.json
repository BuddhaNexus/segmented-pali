{
  "iti10:0.1_0": "Itivuttaka 10",
  "iti10:0.2_0": "Ekakanipāta",
  "iti10:0.3_0": "Paṭhamavagga",
  "iti10:0.4_0": "Dosapariññāsutta",
  "iti10:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti10:2.1_0": "“Dosaṁ, bhikkhave,",
  "iti10:2.1_1": "anabhijānaṁ aparijānaṁ tattha cittaṁ avirājayaṁ appajahaṁ abhabbo dukkhakkhayāya.",
  "iti10:2.2_0": "Dosañca kho, bhikkhave,",
  "iti10:2.2_1": "abhijānaṁ parijānaṁ tattha cittaṁ virājayaṁ pajahaṁ bhabbo dukkhakkhayāyā”ti.",
  "iti10:2.3_0": "Etamatthaṁ bhagavā avoca.",
  "iti10:2.4_0": "Tatthetaṁ iti vuccati:",
  "iti10:3.1_0": "“Yena dosena duṭṭhāse,",
  "iti10:3.2_0": "sattā gacchanti duggatiṁ;",
  "iti10:3.3_0": "Taṁ dosaṁ sammadaññāya,",
  "iti10:3.4_0": "pajahanti vipassino;",
  "iti10:3.5_0": "Pahāya na punāyanti,",
  "iti10:3.6_0": "imaṁ lokaṁ kudācanan”ti.",
  "iti10:4.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti10:4.2_0": "Dasamaṁ.",
  "iti10:4.3_0": "Pāṭibhogavaggo paṭhamo.",
  "iti10:5.0_0": "Tassuddānaṁ",
  "iti10:5.1_0": "Rāgadosā atha moho,",
  "iti10:5.2_0": "Kodhamakkhā mānaṁ sabbaṁ;",
  "iti10:5.3_0": "Mānato rāgadosā puna dve,",
  "iti10:5.4_0": "Pakāsitā vaggamāhu paṭhamanti."
}