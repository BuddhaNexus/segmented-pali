{
  "ja414:0.1_0": "Jātaka",
  "ja414:0.2_0": "Sattakanipāta",
  "ja414:0.3_0": "Gandhāravagga",
  "ja414:0.4_0": "9. Jāgarajātaka",
  "ja414:1.1_0": "“Kodha jāgarataṁ sutto,",
  "ja414:1.2_0": "kodha suttesu jāgaro;",
  "ja414:1.3_0": "Ko mametaṁ vijānāti,",
  "ja414:1.4_0": "ko taṁ paṭibhaṇāti me”.",
  "ja414:2.1_0": "“Ahaṁ jāgarataṁ sutto,",
  "ja414:2.2_0": "ahaṁ suttesu jāgaro;",
  "ja414:2.3_0": "Ahametaṁ vijānāmi,",
  "ja414:2.4_0": "ahaṁ paṭibhaṇāmi te”.",
  "ja414:3.1_0": "“Kathaṁ jāgarataṁ sutto,",
  "ja414:3.2_0": "kathaṁ suttesu jāgaro;",
  "ja414:3.3_0": "Kathaṁ etaṁ vijānāsi,",
  "ja414:3.4_0": "kathaṁ paṭibhaṇāsi me”.",
  "ja414:4.1_0": "“Ye dhammaṁ nappajānanti,",
  "ja414:4.2_0": "saṁyamoti damoti ca;",
  "ja414:4.3_0": "Tesu suppamānesu,",
  "ja414:4.4_0": "ahaṁ jaggāmi devate.",
  "ja414:5.1_0": "Yesaṁ rāgo ca doso ca,",
  "ja414:5.2_0": "avijjā ca virājitā;",
  "ja414:5.3_0": "Tesu jāgaramānesu,",
  "ja414:5.4_0": "ahaṁ suttosmi devate.",
  "ja414:6.1_0": "Evaṁ jāgarataṁ sutto,",
  "ja414:6.2_0": "evaṁ suttesu jāgaro;",
  "ja414:6.3_0": "Evametaṁ vijānāmi,",
  "ja414:6.4_0": "evaṁ paṭibhaṇāmi te”.",
  "ja414:7.1_0": "“Sādhu jāgarataṁ sutto,",
  "ja414:7.2_0": "sādhu suttesu jāgaro;",
  "ja414:7.3_0": "Sādhumetaṁ vijānāsi,",
  "ja414:7.4_0": "sādhu paṭibhaṇāsi me”ti.",
  "ja414:8.1_0": "Jāgarajātakaṁ navamaṁ."
}