{
  "ja438:0.1_0": "Jātaka",
  "ja438:0.2_0": "Navakanipāta",
  "ja438:0.3_0": "Gijjhavagga",
  "ja438:0.4_0": "12. Daddarajātaka",
  "ja438:1.1_0": "“Yo te puttake akhādi,",
  "ja438:1.2_0": "dinnabhatto adūsake;",
  "ja438:1.3_0": "Tasmiṁ dāṭhaṁ nipātehi,",
  "ja438:1.4_0": "mā te muccittha jīvato”.",
  "ja438:2.1_0": "“Ākiṇṇaluddo puriso,",
  "ja438:2.2_0": "dhāticelaṁva makkhito;",
  "ja438:2.3_0": "Padesaṁ taṁ na passāmi,",
  "ja438:2.4_0": "yattha dāṭhaṁ nipātaye.",
  "ja438:3.1_0": "Akataññussa posassa,",
  "ja438:3.2_0": "niccaṁ vivaradassino;",
  "ja438:3.3_0": "Sabbañce pathaviṁ dajjā,",
  "ja438:3.4_0": "neva naṁ abhirādhaye”.",
  "ja438:4.1_0": "“Kiṁ nu subāhu taramānarūpo,",
  "ja438:4.2_0": "Paccāgatosi saha māṇavena;",
  "ja438:4.3_0": "Kiṁ kiccamatthaṁ idhamatthi tuyhaṁ,",
  "ja438:4.4_0": "Akkhāhi me pucchito etamatthaṁ”.",
  "ja438:5.1_0": "“Yo te sakhā daddaro sādhurūpo,",
  "ja438:5.2_0": "Tassa vadhaṁ parisaṅkāmi ajja;",
  "ja438:5.3_0": "Purisassa kammāyatanāni sutvā,",
  "ja438:5.4_0": "Nāhaṁ sukhiṁ daddaraṁ ajja maññe”.",
  "ja438:6.1_0": "“Kānissa kammāyatanāni assu,",
  "ja438:6.2_0": "Purisassa vuttisamodhānatāya;",
  "ja438:6.3_0": "Kaṁ vā paṭiññaṁ purisassa sutvā,",
  "ja438:6.4_0": "Parisaṅkasi daddaraṁ māṇavena”.",
  "ja438:7.1_0": "“Ciṇṇā kaliṅgā caritā vaṇijjā,",
  "ja438:7.2_0": "Vettācaro saṅkupathopi ciṇṇo;",
  "ja438:7.3_0": "Naṭehi ciṇṇaṁ saha vākurehi,",
  "ja438:7.4_0": "Daṇḍena yuddhampi samajjamajjhe.",
  "ja438:8.1_0": "Baddhā kulīkā mitamāḷhakena,",
  "ja438:8.2_0": "Akkhā jitā saṁyamo abbhatīto;",
  "ja438:8.3_0": "Abbāhitaṁ pubbakaṁ aḍḍharattaṁ,",
  "ja438:8.4_0": "Hatthā daḍḍhā piṇḍapaṭiggahena.",
  "ja438:9.1_0": "Tānissa kammāyatanāni assu,",
  "ja438:9.2_0": "Purisassa vuttisamodhānatāya;",
  "ja438:9.3_0": "Yathā ayaṁ dissati lomapiṇḍo,",
  "ja438:9.4_0": "Gāvo hatā kiṁ pana daddarassā”ti. ",
  "ja438:10.1_0": "Daddarajātakaṁ dvādasamaṁ.",
  "ja438:11.1_0": "Navakanipātaṁ niṭṭhitaṁ.",
  "ja438:12.1_0": "Tassuddānaṁ",
  "ja438:13.1_0": "Varagijjha samajjana haṁsavaro,",
  "ja438:13.2_0": "Nidhisavhaya hārita pāṭaliko;",
  "ja438:13.3_0": "Ajarāmara dhaṅka titikkha kuto,",
  "ja438:13.4_0": "Atha dvādasa pekkhana daddaribhīti."
}