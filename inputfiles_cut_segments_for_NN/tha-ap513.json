{
  "tha-ap513:0.1_0": "Therāpadāna",
  "tha-ap513:0.2_0": "Phaladāyakavagga",
  "tha-ap513:0.3_0": "1 Kurañciyaphaladāyakattheraapadāna",
  "tha-ap513:1.1_0": "“Migaluddo pure āsiṁ,",
  "tha-ap513:1.2_0": "vipine vicaraṁ ahaṁ;",
  "tha-ap513:1.3_0": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap513:1.4_0": "sabbadhammāna pāraguṁ.",
  "tha-ap513:2.1_0": "Kurañciyaphalaṁ gayha,",
  "tha-ap513:2.2_0": "buddhaseṭṭhassadāsahaṁ;",
  "tha-ap513:2.3_0": "Puññakkhettassa tādino,",
  "tha-ap513:2.4_0": "pasanno sehi pāṇibhi.",
  "tha-ap513:3.1_0": "Ekatiṁse ito kappe,",
  "tha-ap513:3.2_0": "yaṁ phalaṁ adadiṁ tadā;",
  "tha-ap513:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap513:3.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap513:4.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap513:4.2_0": "bhavā sabbe samūhatā;",
  "tha-ap513:4.3_0": "Nāgova bandhanaṁ chetvā,",
  "tha-ap513:4.4_0": "viharāmi anāsavo.",
  "tha-ap513:5.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap513:5.2_0": "buddhaseṭṭhassa santike;",
  "tha-ap513:5.3_0": "Tisso vijjā anuppattā,",
  "tha-ap513:5.4_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap513:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap513:6.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap513:6.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap513:6.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap513:7.1_0": "Itthaṁ sudaṁ āyasmā kurañciyaphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap513:8.1_0": "Kurañciyaphaladāyakattherassāpadānaṁ paṭhamaṁ."
}