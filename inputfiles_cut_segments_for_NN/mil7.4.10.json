{
  "mil7.4.10:0.1_0": "Milindapañha",
  "mil7.4.10:0.2_0": "Opammakathāpañha",
  "mil7.4.10:0.3_0": "Pathavīvagga",
  "mil7.4.10:0.4_0": "10. Cakkavattiṅgapañha",
  "mil7.4.10:1.1_0": "“Bhante nāgasena,",
  "mil7.4.10:1.1_1": "‘cakkavattissa cattāri aṅgāni gahetabbānī’ti yaṁ vadesi,",
  "mil7.4.10:1.1_2": "katamāni tāni cattāri aṅgāni gahetabbānī”ti?",
  "mil7.4.10:1.2_0": "“Yathā, mahārāja,",
  "mil7.4.10:1.2_1": "cakkavattī catūhi saṅgahavatthūhi janaṁ saṅgaṇhāti;",
  "mil7.4.10:1.3_0": "evameva kho, mahārāja,",
  "mil7.4.10:1.3_1": "yoginā yogāvacarena catassannaṁ parisānaṁ mānasaṁ saṅgahetabbaṁ anuggahetabbaṁ sampahaṁsetabbaṁ.",
  "mil7.4.10:1.4_0": "Idaṁ, mahārāja,",
  "mil7.4.10:1.4_1": "cakkavattissa paṭhamaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.4.10:2.1_0": "Puna caparaṁ, mahārāja,",
  "mil7.4.10:2.1_1": "cakkavattissa vijite corā na uṭṭhahanti;",
  "mil7.4.10:2.2_0": "evameva kho, mahārāja,",
  "mil7.4.10:2.2_1": "yoginā yogāvacarena kāmarāgabyāpādavihiṁsāvitakkā na uppādetabbā.",
  "mil7.4.10:2.3_0": "Idaṁ, mahārāja,",
  "mil7.4.10:2.3_1": "cakkavattissa dutiyaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.4.10:2.4_0": "Bhāsitampetaṁ, mahārāja, bhagavatā devātidevena—",
  "mil7.4.10:3.1_0": "‘Vitakkūpasame ca yo rato,",
  "mil7.4.10:3.2_0": "Asubhaṁ bhāvayate sadā sato;",
  "mil7.4.10:3.3_0": "Esa kho byanti kāhiti,",
  "mil7.4.10:3.4_0": "Esa checchati mārabandhanan’ti.",
  "mil7.4.10:4.1_0": "Puna caparaṁ, mahārāja,",
  "mil7.4.10:4.1_1": "cakkavattī divase divase samuddapariyantaṁ mahāpathaviṁ anuyāyati kalyāṇapāpakāni vicinamāno;",
  "mil7.4.10:4.2_0": "evameva kho, mahārāja,",
  "mil7.4.10:4.2_1": "yoginā yogāvacarena kāyakammaṁ vacīkammaṁ manokammaṁ divase divase paccavekkhitabbaṁ ‘kiṁ nu kho me imehi tīhi ṭhānehi anupavajjassa divaso vītivattatī’ti.",
  "mil7.4.10:4.3_0": "Idaṁ, mahārāja,",
  "mil7.4.10:4.3_1": "cakkavattissa tatiyaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.4.10:4.4_0": "Bhāsitampetaṁ, mahārāja,",
  "mil7.4.10:4.4_1": "bhagavatā devātidevena aṅguttaranikāyavare—",
  "mil7.4.10:5.1_0": "‘Kathambhūtassa me rattindivā vītivattantīti pabbajitena abhiṇhaṁ paccavekkhitabban’ti.",
  "mil7.4.10:6.1_0": "Puna caparaṁ, mahārāja,",
  "mil7.4.10:6.1_1": "cakkavattissa abbhantarabāhirārakkhā susaṁvihitā hoti;",
  "mil7.4.10:6.2_0": "evameva kho, mahārāja,",
  "mil7.4.10:6.2_1": "yoginā yogāvacarena abbhantarānaṁ bāhirānaṁ kilesānaṁ ārakkhāya satidovāriko ṭhapetabbo.",
  "mil7.4.10:6.3_0": "Idaṁ, mahārāja,",
  "mil7.4.10:6.3_1": "cakkavattissa catutthaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.4.10:6.4_0": "Bhāsitampetaṁ, mahārāja, bhagavatā devātidevena—",
  "mil7.4.10:7.1_0": "‘Satidovāriko, bhikkhave,",
  "mil7.4.10:7.1_1": "ariyasāvako akusalaṁ pajahati kusalaṁ bhāveti, sāvajjaṁ pajahati, anavajjaṁ bhāveti,",
  "mil7.4.10:7.1_2": "suddhamattānaṁ pariharatī’”ti.",
  "mil7.4.10:8.1_0": "Cakkavattiṅgapañho dasamo.",
  "mil7.4.10:9.1_0": "Pathavīvaggo tatiyo.",
  "mil7.4.10:10.1_0": "Tassuddānaṁ",
  "mil7.4.10:11.1_0": "Pathavī āpo ca tejo ca,",
  "mil7.4.10:11.2_0": "vāyo ca pabbatena ca;",
  "mil7.4.10:11.3_0": "Ākāso candasūriyo ca,",
  "mil7.4.10:11.4_0": "sakko ca cakkavattināti."
}