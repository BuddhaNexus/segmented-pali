{
  "ja55:0.1_0": "Jātaka",
  "ja55:0.2_0": "Ekakanipāta",
  "ja55:0.3_0": "Āsīsavagga",
  "ja55:0.4_0": "5. Pañcāvudhajātaka",
  "ja55:1.1_0": "“Yo alīnena cittena,",
  "ja55:1.2_0": "alīnamanaso naro;",
  "ja55:1.3_0": "Bhāveti kusalaṁ dhammaṁ,",
  "ja55:1.4_0": "yogakkhemassa pattiyā;",
  "ja55:1.5_0": "Pāpuṇe anupubbena,",
  "ja55:1.6_0": "sabbasaṁyojanakkhayan”ti.",
  "ja55:2.1_0": "Pañcāvudhajātakaṁ pañcamaṁ."
}