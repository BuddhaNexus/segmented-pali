{
  "tha-ap121:0.1_0": "Therāpadāna",
  "tha-ap121:0.2_0": "Mahāparivāravagga",
  "tha-ap121:0.3_0": "9. Paccupaṭṭhānasaññakattheraapadāna",
  "tha-ap121:1.1_0": "“Atthadassimhi sugate,",
  "tha-ap121:1.2_0": "nibbute samanantarā;",
  "tha-ap121:1.3_0": "Yakkhayoniṁ upapajjiṁ,",
  "tha-ap121:1.4_0": "yasaṁ patto cahaṁ tadā.",
  "tha-ap121:2.1_0": "Dulladdhaṁ vata me āsi,",
  "tha-ap121:2.2_0": "duppabhātaṁ duruṭṭhitaṁ;",
  "tha-ap121:2.3_0": "Yaṁ me bhoge vijjamāne,",
  "tha-ap121:2.4_0": "parinibbāyi cakkhumā.",
  "tha-ap121:3.1_0": "Mama saṅkappamaññāya,",
  "tha-ap121:3.2_0": "sāgaro nāma sāvako;",
  "tha-ap121:3.3_0": "Mamuddharitukāmo so,",
  "tha-ap121:3.4_0": "āgacchi mama santikaṁ.",
  "tha-ap121:4.1_0": "Kiṁ nu socasi mā bhāyi,",
  "tha-ap121:4.2_0": "cara dhammaṁ sumedhasa;",
  "tha-ap121:4.3_0": "Anuppadinnā buddhena,",
  "tha-ap121:4.4_0": "sabbesaṁ bījasampadā.",
  "tha-ap121:5.1_0": "Yo ce pūjeyya sambuddhaṁ,",
  "tha-ap121:5.2_0": "tiṭṭhantaṁ lokanāyakaṁ;",
  "tha-ap121:5.3_0": "Dhātuṁ sāsapamattampi,",
  "tha-ap121:5.4_0": "nibbutassāpi pūjaye.",
  "tha-ap121:6.1_0": "Same cittappasādamhi,",
  "tha-ap121:6.2_0": "samaṁ puññaṁ mahaggataṁ;",
  "tha-ap121:6.3_0": "Tasmā thūpaṁ karitvāna,",
  "tha-ap121:6.4_0": "pūjehi jinadhātuyo.",
  "tha-ap121:7.1_0": "Sāgarassa vaco sutvā,",
  "tha-ap121:7.2_0": "buddhathūpaṁ akāsahaṁ;",
  "tha-ap121:7.3_0": "Pañcavasse paricariṁ,",
  "tha-ap121:7.4_0": "munino thūpamuttamaṁ.",
  "tha-ap121:8.1_0": "Tena kammena dvipadinda,",
  "tha-ap121:8.2_0": "lokajeṭṭha narāsabha;",
  "tha-ap121:8.3_0": "Sampattiṁ anubhotvāna,",
  "tha-ap121:8.4_0": "arahattamapāpuṇiṁ.",
  "tha-ap121:9.1_0": "Bhūripaññā ca cattāro,",
  "tha-ap121:9.2_0": "sattakappasate ito;",
  "tha-ap121:9.3_0": "Sattaratanasampannā,",
  "tha-ap121:9.4_0": "cakkavattī mahabbalā.",
  "tha-ap121:10.1_0": "Paṭisambhidā catasso,",
  "tha-ap121:10.2_0": "…pe…",
  "tha-ap121:10.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap121:11.1_0": "Itthaṁ sudaṁ āyasmā paccupaṭṭhānasaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap121:12.1_0": "Paccupaṭṭhānasaññakattherassāpadānaṁ navamaṁ."
}