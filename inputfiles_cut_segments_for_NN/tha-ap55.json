{
  "tha-ap55:0.1_0": "Therāpadāna",
  "tha-ap55:0.2_0": "Bījanivagga",
  "tha-ap55:0.3_0": "3. Sayanadāyakattheraapadāna",
  "tha-ap55:1.1_0": "“Padumuttarabuddhassa,",
  "tha-ap55:1.2_0": "sabbalokānukampino;",
  "tha-ap55:1.3_0": "Sayanaṁ tassa pādāsiṁ,",
  "tha-ap55:1.4_0": "vippasannena cetasā.",
  "tha-ap55:2.1_0": "Tena sayanadānena,",
  "tha-ap55:2.2_0": "sukhette bījasampadā;",
  "tha-ap55:2.3_0": "Bhogā nibbattare tassa,",
  "tha-ap55:2.4_0": "sayanassa idaṁ phalaṁ.",
  "tha-ap55:3.1_0": "Ākāse seyyaṁ kappemi,",
  "tha-ap55:3.2_0": "dhāremi pathaviṁ imaṁ;",
  "tha-ap55:3.3_0": "Pāṇesu me issariyaṁ,",
  "tha-ap55:3.4_0": "sayanassa idaṁ phalaṁ.",
  "tha-ap55:4.1_0": "Pañcakappasahassamhi,",
  "tha-ap55:4.2_0": "aṭṭha āsuṁ mahātejā;",
  "tha-ap55:4.3_0": "Catuttiṁse kappasate,",
  "tha-ap55:4.4_0": "caturo ca mahabbalā.",
  "tha-ap55:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap55:5.2_0": "…pe…",
  "tha-ap55:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap55:6.1_0": "Itthaṁ sudaṁ āyasmā sayanadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap55:7.1_0": "Sayanadāyakattherassāpadānaṁ tatiyaṁ."
}