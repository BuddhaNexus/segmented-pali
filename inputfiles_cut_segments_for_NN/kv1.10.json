{
  "kv1.10:0.1_0": "Kathāvatthu",
  "kv1.10:0.2_0": "Mahāpaṇṇāsaka",
  "kv1.10:0.3_0": "Paṭhamavagga",
  "kv1.10:0.4_0": "Hevatthikathā",
  "kv1.10:1.1_0": "Atītaṁ atthīti?",
  "kv1.10:1.2_0": "Hevatthi, heva natthīti.",
  "kv1.10:1.3_0": "Sevatthi, seva natthīti?",
  "kv1.10:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv1.10:1.5_0": "sevatthi, seva natthīti?",
  "kv1.10:1.6_0": "Āmantā.",
  "kv1.10:1.7_0": "Atthaṭṭho natthaṭṭho, natthaṭṭho atthaṭṭho, atthibhāvo natthibhāvo, natthibhāvo atthibhāvo, atthīti vā natthīti vā,",
  "kv1.10:1.7_1": "natthīti vā atthīti vā esese ekaṭṭhe same samabhāge tajjāteti?",
  "kv1.10:1.8_0": "Na hevaṁ vattabbe …pe….",
  "kv1.10:2.1_0": "Anāgataṁ atthīti?",
  "kv1.10:2.2_0": "Hevatthi, heva natthīti.",
  "kv1.10:2.3_0": "Sevatthi, seva natthīti?",
  "kv1.10:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv1.10:2.5_0": "sevatthi, seva natthīti?",
  "kv1.10:2.6_0": "Āmantā.",
  "kv1.10:2.7_0": "Atthaṭṭho natthaṭṭho, natthaṭṭho atthaṭṭho, atthibhāvo natthibhāvo, natthibhāvo atthibhāvo, atthīti vā natthīti vā,",
  "kv1.10:2.7_1": "natthīti vā atthīti vā esese ekaṭṭhe same samabhāge tajjāteti?",
  "kv1.10:2.8_0": "Na hevaṁ vattabbe …pe….",
  "kv1.10:3.1_0": "Paccuppannaṁ atthīti?",
  "kv1.10:3.2_0": "Hevatthi, heva natthīti.",
  "kv1.10:3.3_0": "Sevatthi, seva natthīti?",
  "kv1.10:3.4_0": "Na hevaṁ vattabbe …pe…",
  "kv1.10:3.5_0": "sevatthi, seva natthīti?",
  "kv1.10:3.6_0": "Āmantā.",
  "kv1.10:3.7_0": "Atthaṭṭho natthaṭṭho …pe…",
  "kv1.10:3.8_0": "same samabhāge tajjāteti?",
  "kv1.10:3.9_0": "Na hevaṁ vattabbe …pe….",
  "kv1.10:4.1_0": "Atītaṁ hevatthi, heva natthīti?",
  "kv1.10:4.2_0": "Āmantā.",
  "kv1.10:4.3_0": "Kintatthi, kinti natthīti?",
  "kv1.10:4.4_0": "Atītaṁ atītanti hevatthi,",
  "kv1.10:4.4_1": "atītaṁ anāgatanti heva natthi,",
  "kv1.10:4.4_2": "atītaṁ paccuppannanti heva natthīti.",
  "kv1.10:4.5_0": "Sevatthi, seva natthīti?",
  "kv1.10:4.6_0": "Na hevaṁ vattabbe …pe…",
  "kv1.10:4.7_0": "sevatthi, seva natthīti?",
  "kv1.10:4.8_0": "Āmantā.",
  "kv1.10:4.9_0": "Atthaṭṭho natthaṭṭho, natthaṭṭho atthaṭṭho, atthibhāvo natthibhāvo, natthibhāvo atthibhāvo, atthīti vā natthīti vā,",
  "kv1.10:4.9_1": "natthīti vā atthīti vā esese ekaṭṭhe same samabhāge tajjāteti?",
  "kv1.10:4.10_0": "Na hevaṁ vattabbe …pe….",
  "kv1.10:5.1_0": "Anāgataṁ hevatthi, heva natthīti?",
  "kv1.10:5.2_0": "Āmantā.",
  "kv1.10:5.3_0": "Kintatthi, kinti natthīti?",
  "kv1.10:5.4_0": "Anāgataṁ anāgatanti hevatthi,",
  "kv1.10:5.4_1": "anāgataṁ atītanti heva natthi,",
  "kv1.10:5.4_2": "anāgataṁ paccuppannanti heva natthīti.",
  "kv1.10:5.5_0": "Sevatthi, seva natthīti?",
  "kv1.10:5.6_0": "Na hevaṁ vattabbe …pe…",
  "kv1.10:5.7_0": "sevatthi, seva natthīti?",
  "kv1.10:5.8_0": "Āmantā.",
  "kv1.10:5.9_0": "Atthaṭṭho natthaṭṭho, natthaṭṭho atthaṭṭho …pe…",
  "kv1.10:5.10_0": "same samabhāge tajjāteti?",
  "kv1.10:5.11_0": "Na hevaṁ vattabbe …pe….",
  "kv1.10:6.1_0": "Paccuppannaṁ hevatthi, heva natthīti?",
  "kv1.10:6.2_0": "Āmantā.",
  "kv1.10:6.3_0": "Kintatthi, kinti natthīti?",
  "kv1.10:6.4_0": "Paccuppannaṁ paccuppannanti hevatthi,",
  "kv1.10:6.4_1": "paccuppannaṁ atītanti heva natthi,",
  "kv1.10:6.4_2": "paccuppannaṁ anāgatanti heva natthīti.",
  "kv1.10:6.5_0": "Sevatthi, seva natthīti?",
  "kv1.10:6.6_0": "Na hevaṁ vattabbe …pe…",
  "kv1.10:6.7_0": "sevatthi, seva natthīti?",
  "kv1.10:6.8_0": "Āmantā.",
  "kv1.10:6.9_0": "Atthaṭṭho natthaṭṭho …pe…",
  "kv1.10:6.10_0": "same samabhāge tajjāteti?",
  "kv1.10:6.11_0": "Na hevaṁ vattabbe …pe….",
  "kv1.10:7.1_0": "Na vattabbaṁ—",
  "kv1.10:7.2_0": "“atītaṁ hevatthi, heva natthi;",
  "kv1.10:7.3_0": "anāgataṁ hevatthi, heva natthi;",
  "kv1.10:7.4_0": "paccuppannaṁ hevatthi, heva natthī”ti?",
  "kv1.10:7.5_0": "Āmantā.",
  "kv1.10:7.6_0": "Atītaṁ anāgatanti hevatthi,",
  "kv1.10:7.6_1": "atītaṁ paccuppannanti hevatthi, anāgataṁ atītanti hevatthi,",
  "kv1.10:7.6_2": "anāgataṁ paccuppannanti hevatthi,",
  "kv1.10:7.6_3": "paccuppannaṁ atītanti hevatthi,",
  "kv1.10:7.6_4": "paccuppannaṁ anāgatanti hevatthīti?",
  "kv1.10:7.7_0": "Na hevaṁ vattabbe.",
  "kv1.10:7.8_0": "…pe….",
  "kv1.10:7.9_0": "Tena hi atītaṁ hevatthi heva natthi,",
  "kv1.10:7.9_1": "anāgataṁ hevatthi heva natthi, paccuppannaṁ hevatthi, heva natthīti.",
  "kv1.10:8.1_0": "Rūpaṁ atthīti?",
  "kv1.10:8.2_0": "Hevatthi, heva natthīti.",
  "kv1.10:8.3_0": "Sevatthi, seva natthīti?",
  "kv1.10:8.4_0": "Na hevaṁ vattabbe …pe…",
  "kv1.10:8.5_0": "sevatthi, seva natthīti?",
  "kv1.10:8.6_0": "Āmantā.",
  "kv1.10:8.7_0": "Atthaṭṭho natthaṭṭho, natthaṭṭho atthaṭṭho, atthibhāvo natthibhāvo, natthibhāvo atthibhāvo, atthīti vā natthīti vā,",
  "kv1.10:8.7_1": "natthīti vā atthīti vā esese ekaṭṭhe same samabhāge tajjāteti?",
  "kv1.10:8.8_0": "Na hevaṁ vattabbe …pe….",
  "kv1.10:9.1_0": "Vedanā …",
  "kv1.10:9.2_0": "saññā …",
  "kv1.10:9.3_0": "saṅkhārā …",
  "kv1.10:9.4_0": "viññāṇaṁ atthīti?",
  "kv1.10:9.5_0": "Hevatthi, heva natthīti.",
  "kv1.10:9.6_0": "Sevatthi, seva natthīti?",
  "kv1.10:9.7_0": "Na hevaṁ vattabbe …pe…",
  "kv1.10:9.8_0": "sevatthi, seva natthīti?",
  "kv1.10:9.9_0": "Āmantā.",
  "kv1.10:9.10_0": "Atthaṭṭho natthaṭṭho …pe…",
  "kv1.10:9.11_0": "same samabhāge tajjāteti?",
  "kv1.10:9.12_0": "Na hevaṁ vattabbe …pe….",
  "kv1.10:10.1_0": "Rūpaṁ hevatthi, heva natthīti?",
  "kv1.10:10.2_0": "Āmantā.",
  "kv1.10:10.3_0": "Kintatthi, kinti natthīti?",
  "kv1.10:10.4_0": "Rūpaṁ rūpanti hevatthi,",
  "kv1.10:10.4_1": "rūpaṁ vedanāti heva natthi …pe…",
  "kv1.10:10.5_0": "rūpaṁ saññāti heva natthi …pe…",
  "kv1.10:10.6_0": "rūpaṁ saṅkhārāti heva natthi …pe…",
  "kv1.10:10.7_0": "rūpaṁ viññāṇanti heva natthīti.",
  "kv1.10:10.8_0": "Sevatthi, seva natthīti?",
  "kv1.10:10.9_0": "Na hevaṁ vattabbe …pe…",
  "kv1.10:10.10_0": "sevatthi, seva natthīti?",
  "kv1.10:10.11_0": "Āmantā.",
  "kv1.10:10.12_0": "Atthaṭṭho natthaṭṭho …pe…",
  "kv1.10:10.13_0": "same samabhāge tajjāteti?",
  "kv1.10:10.14_0": "Na hevaṁ vattabbe …pe….",
  "kv1.10:11.1_0": "Vedanā …",
  "kv1.10:11.2_0": "saññā …",
  "kv1.10:11.3_0": "saṅkhārā …",
  "kv1.10:11.4_0": "viññāṇaṁ hevatthi, heva natthīti?",
  "kv1.10:11.5_0": "Āmantā.",
  "kv1.10:11.6_0": "Kintatthi, kinti natthīti?",
  "kv1.10:11.7_0": "Viññāṇaṁ viññāṇanti hevatthi.",
  "kv1.10:11.8_0": "Viññāṇaṁ rūpanti heva natthi …pe…",
  "kv1.10:11.9_0": "viññāṇaṁ vedanāti heva natthi …pe…",
  "kv1.10:11.10_0": "viññāṇaṁ saññāti heva natthi …pe…",
  "kv1.10:11.11_0": "viññāṇaṁ saṅkhārāti heva natthīti.",
  "kv1.10:11.12_0": "Sevatthi, seva natthīti?",
  "kv1.10:11.13_0": "Na hevaṁ vattabbe …pe…",
  "kv1.10:11.14_0": "sevatthi, seva natthīti?",
  "kv1.10:11.15_0": "Āmantā.",
  "kv1.10:11.16_0": "Atthaṭṭho natthaṭṭho …pe…",
  "kv1.10:11.17_0": "same samabhāge tajjāteti?",
  "kv1.10:11.18_0": "Na hevaṁ vattabbe …pe….",
  "kv1.10:12.1_0": "Na vattabbaṁ—",
  "kv1.10:12.2_0": "“rūpaṁ hevatthi, heva natthīti;",
  "kv1.10:12.3_0": "vedanā …",
  "kv1.10:12.4_0": "saññā …",
  "kv1.10:12.5_0": "saṅkhārā …",
  "kv1.10:12.6_0": "viññāṇaṁ hevatthi, heva natthīti?",
  "kv1.10:12.7_0": "Āmantā.",
  "kv1.10:12.8_0": "Rūpaṁ vedanāti hevatthi …pe…",
  "kv1.10:12.9_0": "rūpaṁ saññāti hevatthi …pe…",
  "kv1.10:12.10_0": "rūpaṁ saṅkhārāti hevatthi …pe…",
  "kv1.10:12.11_0": "rūpaṁ viññāṇanti hevatthi …",
  "kv1.10:12.12_0": "vedanā …",
  "kv1.10:12.13_0": "saññā …",
  "kv1.10:12.14_0": "saṅkhārā …",
  "kv1.10:12.15_0": "viññāṇaṁ rūpanti hevatthi …",
  "kv1.10:12.16_0": "viññāṇaṁ vedanāti hevatthi …",
  "kv1.10:12.17_0": "viññāṇaṁ saññāti hevatthi …",
  "kv1.10:12.18_0": "viññāṇaṁ saṅkhārāti hevatthīti?",
  "kv1.10:12.19_0": "Na hevaṁ vattabbe …pe…",
  "kv1.10:12.20_0": "tena hi rūpaṁ hevatthi, heva natthi;",
  "kv1.10:12.21_0": "vedanā …",
  "kv1.10:12.22_0": "saññā …",
  "kv1.10:12.23_0": "saṅkhārā …",
  "kv1.10:12.24_0": "viññāṇaṁ hevatthi, heva natthīti.",
  "kv1.10:12.25_0": "Hevatthikathā niṭṭhitā.",
  "kv1.10:12.26_0": "Tassuddānaṁ",
  "kv1.10:13.1_0": "Upalabbho parihāni,",
  "kv1.10:13.2_0": "Brahmacariyavāso odhiso;",
  "kv1.10:13.3_0": "Pariññā kāmarāgappahānaṁ,",
  "kv1.10:13.4_0": "Sabbatthivādo āyatanaṁ;",
  "kv1.10:13.5_0": "Atītānāgato subhaṅgo,",
  "kv1.10:13.6_0": "Sabbe dhammā satipaṭṭhānā;",
  "kv1.10:13.7_0": "Hevatthi heva natthīti.",
  "kv1.10:13.8_0": "Mahāvaggo."
}