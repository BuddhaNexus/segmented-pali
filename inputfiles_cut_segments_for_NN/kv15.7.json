{
  "kv15.7:0.1_0": "Kathāvatthu",
  "kv15.7:0.2_0": "Mahāpaṇṇāsaka",
  "kv15.7:0.3_0": "Pannarasamavagga",
  "kv15.7:0.4_0": "Saññāvedayitakathā",
  "kv15.7:1.1_0": "Saññāvedayitanirodhasamāpatti lokuttarāti?",
  "kv15.7:1.2_0": "Āmantā.",
  "kv15.7:1.3_0": "Maggo phalaṁ nibbānaṁ,",
  "kv15.7:1.3_1": "sotāpattimaggo sotāpattiphalaṁ …pe… bojjhaṅgoti?",
  "kv15.7:1.4_0": "Na hevaṁ vattabbe …pe….",
  "kv15.7:2.1_0": "Na vattabbaṁ—",
  "kv15.7:2.2_0": "“saññāvedayitanirodhasamāpatti lokuttarā”ti?",
  "kv15.7:2.3_0": "Āmantā.",
  "kv15.7:2.4_0": "Lokiyāti?",
  "kv15.7:2.5_0": "Na hevaṁ vattabbe.",
  "kv15.7:2.6_0": "Tena hi lokuttarāti.",
  "kv15.7:2.7_0": "Saññāvedayitakathā niṭṭhitā."
}