{
  "iti98:0.1_0": "Itivuttaka 98",
  "iti98:0.2_0": "Tikanipāta",
  "iti98:0.3_0": "Pañcamavagga",
  "iti98:0.4_0": "Dānasutta",
  "iti98:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti98:2.1_0": "“Dvemāni, bhikkhave, dānāni—",
  "iti98:2.2_0": "āmisadānañca dhammadānañca.",
  "iti98:2.3_0": "Etadaggaṁ, bhikkhave,",
  "iti98:2.3_1": "imesaṁ dvinnaṁ dānānaṁ yadidaṁ dhammadānaṁ.",
  "iti98:3.1_0": "Dveme, bhikkhave, saṁvibhāgā—",
  "iti98:3.2_0": "āmisasaṁvibhāgo ca dhammasaṁvibhāgo ca.",
  "iti98:3.3_0": "Etadaggaṁ, bhikkhave,",
  "iti98:3.3_1": "imesaṁ dvinnaṁ saṁvibhāgānaṁ yadidaṁ dhammasaṁvibhāgo.",
  "iti98:4.1_0": "Dveme, bhikkhave, anuggahā—",
  "iti98:4.2_0": "āmisānuggaho ca dhammānuggaho ca.",
  "iti98:4.3_0": "Etadaggaṁ, bhikkhave,",
  "iti98:4.3_1": "imesaṁ dvinnaṁ anuggahānaṁ yadidaṁ dhammānuggaho”ti.",
  "iti98:4.4_0": "Etamatthaṁ bhagavā avoca.",
  "iti98:4.5_0": "Tatthetaṁ iti vuccati:",
  "iti98:5.1_0": "“Yamāhu dānaṁ paramaṁ anuttaraṁ,",
  "iti98:5.2_0": "Yaṁ saṁvibhāgaṁ bhagavā avaṇṇayi;",
  "iti98:5.3_0": "Aggamhi khettamhi pasannacitto,",
  "iti98:5.4_0": "Viññū pajānaṁ ko na yajetha kāle.",
  "iti98:6.1_0": "Ye ceva bhāsanti suṇanti cūbhayaṁ,",
  "iti98:6.2_0": "Pasannacittā sugatassa sāsane;",
  "iti98:6.3_0": "Tesaṁ so attho paramo visujjhati,",
  "iti98:6.4_0": "Ye appamattā sugatassa sāsane”ti.",
  "iti98:7.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti98:7.2_0": "Navamaṁ."
}