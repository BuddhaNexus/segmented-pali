{
  "iti104:0.1_0": "Itivuttaka 104",
  "iti104:0.2_0": "Catukkanipāta",
  "iti104:0.3_0": "Brāhmaṇadhammayāgavagga",
  "iti104:0.4_0": "Sīlasampannasutta",
  "iti104:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti104:2.1_0": "“Ye te, bhikkhave,",
  "iti104:2.1_1": "bhikkhū sīlasampannā samādhisampannā paññāsampannā vimuttisampannā vimuttiñāṇadassanasampannā ovādakā viññāpakā sandassakā samādapakā samuttejakā sampahaṁsakā alaṁsamakkhātāro saddhammassa dassanampahaṁ, bhikkhave,",
  "iti104:2.1_2": "tesaṁ bhikkhūnaṁ bahūpakāraṁ vadāmi;",
  "iti104:2.2_0": "savanampahaṁ, bhikkhave,",
  "iti104:2.2_1": "tesaṁ bhikkhūnaṁ bahūpakāraṁ vadāmi;",
  "iti104:2.3_0": "upasaṅkamanampahaṁ, bhikkhave,",
  "iti104:2.3_1": "tesaṁ bhikkhūnaṁ bahūpakāraṁ vadāmi;",
  "iti104:2.4_0": "payirupāsanampahaṁ, bhikkhave,",
  "iti104:2.4_1": "tesaṁ bhikkhūnaṁ bahūpakāraṁ vadāmi;",
  "iti104:2.5_0": "anussaraṇampahaṁ, bhikkhave,",
  "iti104:2.5_1": "tesaṁ bhikkhūnaṁ bahūpakāraṁ vadāmi;",
  "iti104:2.6_0": "anupabbajjampahaṁ, bhikkhave,",
  "iti104:2.6_1": "tesaṁ bhikkhūnaṁ bahūpakāraṁ vadāmi.",
  "iti104:2.7_0": "Taṁ kissa hetu?",
  "iti104:2.8_0": "Tathārūpe, bhikkhave,",
  "iti104:2.8_1": "bhikkhū sevato bhajato payirupāsato aparipūropi sīlakkhandho bhāvanāpāripūriṁ gacchati,",
  "iti104:2.8_2": "aparipūropi samādhikkhandho bhāvanāpāripūriṁ gacchati,",
  "iti104:2.8_3": "aparipūropi paññākkhandho bhāvanāpāripūriṁ gacchati,",
  "iti104:2.8_4": "aparipūropi vimuttikkhandho bhāvanāpāripūriṁ gacchati,",
  "iti104:2.8_5": "aparipūropi vimuttiñāṇadassanakkhandho bhāvanāpāripūriṁ gacchati.",
  "iti104:2.9_0": "Evarūpā ca te, bhikkhave,",
  "iti104:2.9_1": "bhikkhū satthārotipi vuccanti, satthavāhātipi vuccanti, raṇañjahātipi vuccanti, tamonudātipi vuccanti, ālokakarātipi vuccanti, obhāsakarātipi vuccanti, pajjotakarātipi vuccanti, ukkādhārātipi vuccanti, pabhaṅkarātipi vuccanti, ariyātipi vuccanti, cakkhumantotipi vuccantī”ti.",
  "iti104:2.10_0": "Etamatthaṁ bhagavā avoca.",
  "iti104:2.11_0": "Tatthetaṁ iti vuccati:",
  "iti104:3.1_0": "“Pāmojjakaraṇaṁ ṭhānaṁ,",
  "iti104:3.2_0": "etaṁ hoti vijānataṁ;",
  "iti104:3.3_0": "Yadidaṁ bhāvitattānaṁ,",
  "iti104:3.4_0": "ariyānaṁ dhammajīvinaṁ.",
  "iti104:4.1_0": "Te jotayanti saddhammaṁ,",
  "iti104:4.2_0": "bhāsayanti pabhaṅkarā;",
  "iti104:4.3_0": "Ālokakaraṇā dhīrā,",
  "iti104:4.4_0": "cakkhumanto raṇañjahā.",
  "iti104:5.1_0": "Yesaṁ ve sāsanaṁ sutvā,",
  "iti104:5.2_0": "sammadaññāya paṇḍitā;",
  "iti104:5.3_0": "Jātikkhayamabhiññāya,",
  "iti104:5.4_0": "nāgacchanti punabbhavan”ti.",
  "iti104:6.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti104:6.2_0": "Pañcamaṁ."
}