{
  "tha-ap89:0.1_0": "Therāpadāna",
  "tha-ap89:0.2_0": "Timiravagga",
  "tha-ap89:0.3_0": "7. Phaladāyakattheraapadāna",
  "tha-ap89:1.1_0": "“Pabbate himavantamhi,",
  "tha-ap89:1.2_0": "kharājinadharo ahaṁ;",
  "tha-ap89:1.3_0": "Phussaṁ jinavaraṁ disvā,",
  "tha-ap89:1.4_0": "phalahattho phalaṁ adaṁ.",
  "tha-ap89:2.1_0": "Yamahaṁ phalamadāsiṁ,",
  "tha-ap89:2.2_0": "vippasannena cetasā;",
  "tha-ap89:2.3_0": "Bhave nibbattamānamhi,",
  "tha-ap89:2.4_0": "phalaṁ nibbattate mama.",
  "tha-ap89:3.1_0": "Dvenavute ito kappe,",
  "tha-ap89:3.2_0": "yaṁ phalaṁ adadiṁ ahaṁ;",
  "tha-ap89:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap89:3.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap89:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap89:4.2_0": "…pe…",
  "tha-ap89:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap89:5.1_0": "Itthaṁ sudaṁ āyasmā phaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap89:6.1_0": "Phaladāyakattherassāpadānaṁ sattamaṁ."
}