{
  "mil5.2.2:0.1_0": "Milindapañha",
  "mil5.2.2:0.2_0": "Meṇḍakapañha",
  "mil5.2.2:0.3_0": "Abhejjavagga",
  "mil5.2.2:0.4_0": "2. Abyākaraṇīyapañha",
  "mil5.2.2:1.1_0": "“Bhante nāgasena, bhāsitampetaṁ bhagavatā—",
  "mil5.2.2:1.2_0": "‘natthānanda,",
  "mil5.2.2:1.2_1": "tathāgatassa dhammesu ācariyamuṭṭhī’ti,",
  "mil5.2.2:1.2_2": "puna ca therena mālukyaputtena pañhaṁ puṭṭho na byākāsi.",
  "mil5.2.2:1.3_0": "Eso kho, bhante nāgasena,",
  "mil5.2.2:1.3_1": "pañho dvayanto ekantanissito bhavissati ajānanena vā guyhakaraṇena vā.",
  "mil5.2.2:1.4_0": "Yadi, bhante nāgasena, bhagavatā bhaṇitaṁ—",
  "mil5.2.2:1.5_0": "‘natthānanda,",
  "mil5.2.2:1.5_1": "tathāgatassa dhammesu ācariyamuṭṭhī’ti,",
  "mil5.2.2:1.5_2": "tena hi therassa mālukyaputtassa ajānantena na byākataṁ.",
  "mil5.2.2:1.6_0": "Yadi jānantena na byākataṁ,",
  "mil5.2.2:1.6_1": "tena hi atthi tathāgatassa dhammesu ācariyamuṭṭhi.",
  "mil5.2.2:1.7_0": "Ayampi ubhato koṭiko pañho tavānuppatto, so tayā nibbāhitabbo”ti.",
  "mil5.2.2:2.1_0": "“Bhāsitampetaṁ, mahārāja, bhagavatā ‘natthānanda,",
  "mil5.2.2:2.1_1": "tathāgatassa dhammesu ācariyamuṭṭhī’ti,",
  "mil5.2.2:2.1_2": "abyākato ca therena mālukyaputtena pucchito pañho,",
  "mil5.2.2:2.1_3": "tañca pana na ajānantena na guyhakaraṇena.",
  "mil5.2.2:2.2_0": "Cattārimāni, mahārāja, pañhabyākaraṇāni.",
  "mil5.2.2:2.3_0": "Katamāni cattāri?",
  "mil5.2.2:2.4_0": "Ekaṁsabyākaraṇīyo pañho vibhajjabyākaraṇīyo pañho paṭipucchābyākaraṇīyo pañho ṭhapanīyo pañhoti.",
  "mil5.2.2:3.1_0": "Katamo ca, mahārāja, ekaṁsabyākaraṇīyo pañho?",
  "mil5.2.2:3.2_0": "‘Rūpaṁ aniccan’ti ekaṁsabyākaraṇīyo pañho,",
  "mil5.2.2:3.3_0": "‘vedanā aniccā’ti …pe…",
  "mil5.2.2:3.4_0": "‘saññā aniccā’ti …pe…",
  "mil5.2.2:3.5_0": "‘saṅkhārā aniccā’ti …pe…",
  "mil5.2.2:3.6_0": "‘viññāṇaṁ aniccan’ti ekaṁsabyākaraṇīyo pañho, ayaṁ ekaṁsabyākaraṇīyo pañho.",
  "mil5.2.2:4.1_0": "Katamo vibhajjabyākaraṇīyo pañho?",
  "mil5.2.2:4.2_0": "‘Aniccaṁ pana rūpan’ti vibhajjabyākaraṇīyo pañho,",
  "mil5.2.2:4.3_0": "‘aniccā pana vedanā’ti …pe…",
  "mil5.2.2:4.4_0": "‘aniccā pana saññā’ti …pe…",
  "mil5.2.2:4.5_0": "‘aniccā pana saṅkhārā’ti …pe…",
  "mil5.2.2:4.6_0": "‘aniccaṁ pana viññāṇan’ti vibhajjabyākaraṇīyo pañho,",
  "mil5.2.2:4.6_1": "ayaṁ vibhajjabyākaraṇīyo pañho.",
  "mil5.2.2:5.1_0": "Katamo paṭipucchābyākaraṇīyo pañho?",
  "mil5.2.2:5.2_0": "‘Kiṁ nu kho cakkhunā sabbaṁ vijānātī’ti ayaṁ paṭipucchābyākaraṇīyo pañho.",
  "mil5.2.2:6.1_0": "Katamo ṭhapanīyo pañho?",
  "mil5.2.2:6.2_0": "‘Sassato loko’ti ṭhapanīyo pañho,",
  "mil5.2.2:6.3_0": "‘asassato loko’ti,",
  "mil5.2.2:6.4_0": "‘antavā loko’ti,",
  "mil5.2.2:6.5_0": "‘anantavā loko’ti,",
  "mil5.2.2:6.6_0": "‘antavā ca anantavā ca loko’ti,",
  "mil5.2.2:6.7_0": "‘nevantavā nānantavā loko’ti,",
  "mil5.2.2:6.8_0": "‘taṁ jīvaṁ taṁ sarīran’ti,",
  "mil5.2.2:6.9_0": "‘aññaṁ jīvaṁ aññaṁ sarīran’ti,",
  "mil5.2.2:6.10_0": "‘hoti tathāgato paraṁ maraṇā’ti,",
  "mil5.2.2:6.11_0": "‘na hoti tathāgato paraṁ maraṇā’ti,",
  "mil5.2.2:6.12_0": "‘hoti ca na ca hoti tathāgato paraṁ maraṇā’ti,",
  "mil5.2.2:6.13_0": "‘neva hoti na na hoti tathāgato paraṁ maraṇā’ti ṭhapanīyo pañho,",
  "mil5.2.2:6.14_0": "ayaṁ ṭhapanīyo pañho.",
  "mil5.2.2:7.1_0": "Bhagavā, mahārāja,",
  "mil5.2.2:7.1_1": "therassa mālukyaputtassa taṁ ṭhapanīyaṁ pañhaṁ na byākāsi.",
  "mil5.2.2:7.2_0": "So pana pañho kiṅkāraṇā ṭhapanīyo?",
  "mil5.2.2:7.3_0": "Na tassa dīpanāya hetu vā kāraṇaṁ vā atthi, tasmā so pañho ṭhapanīyo.",
  "mil5.2.2:7.4_0": "Natthi buddhānaṁ bhagavantānaṁ akāraṇamahetukaṁ giramudīraṇan”ti.",
  "mil5.2.2:7.5_0": "“Sādhu, bhante nāgasena,",
  "mil5.2.2:7.5_1": "evametaṁ tathā sampaṭicchāmī”ti.",
  "mil5.2.2:8.1_0": "Abyākaraṇīyapañho dutiyo."
}