{
  "kv23.1:0.1_0": "Kathāvatthu",
  "kv23.1:0.2_0": "Khuddakaaḍḍhapaṇṇāsaka",
  "kv23.1:0.3_0": "Tevīsatimavagga",
  "kv23.1:0.4_0": "Ekādhippāyakathā",
  "kv23.1:1.1_0": "Ekādhippāyena methuno dhammo paṭisevitabboti?",
  "kv23.1:1.2_0": "Āmantā.",
  "kv23.1:1.3_0": "Ekādhippāyena assamaṇena hotabbaṁ, abhikkhunā hotabbaṁ,",
  "kv23.1:1.3_1": "chinnamūlena hotabbaṁ pārājikena hotabbanti?",
  "kv23.1:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv23.1:1.5_0": "ekādhippāyena methuno dhammo paṭisevitabboti?",
  "kv23.1:1.6_0": "Āmantā.",
  "kv23.1:1.7_0": "Ekādhippāyena pāṇo hantabbo,",
  "kv23.1:1.8_0": "adinnaṁ ādiyitabbaṁ,",
  "kv23.1:1.9_0": "musā bhaṇitabbā,",
  "kv23.1:1.10_0": "pisuṇaṁ bhaṇitabbaṁ,",
  "kv23.1:1.11_0": "pharusaṁ bhaṇitabbaṁ,",
  "kv23.1:1.12_0": "samphaṁ palapitabbaṁ,",
  "kv23.1:1.13_0": "sandhi cheditabbo,",
  "kv23.1:1.14_0": "nillopaṁ hātabbaṁ,",
  "kv23.1:1.15_0": "ekāgārikaṁ kātabbaṁ,",
  "kv23.1:1.16_0": "paripanthe ṭhātabbaṁ,",
  "kv23.1:1.17_0": "paradāro gantabbo,",
  "kv23.1:1.18_0": "gāmaghātako kātabbo,",
  "kv23.1:1.19_0": "nigamaghātako kātabboti?",
  "kv23.1:1.20_0": "Na hevaṁ vattabbe …pe….",
  "kv23.1:1.21_0": "Ekādhippāyakathā niṭṭhitā."
}