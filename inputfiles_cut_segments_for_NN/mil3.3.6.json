{
  "mil3.3.6:0.1_0": "Milindapañha",
  "mil3.3.6:0.2_0": "Vicāravagga",
  "mil3.3.6:0.3_0": "6. Vedagūpañha",
  "mil3.3.6:1.1_0": "Rājā āha—",
  "mil3.3.6:1.2_0": "“bhante nāgasena, vedagū upalabbhatī”ti?",
  "mil3.3.6:1.3_0": "“Ko panesa, mahārāja, vedagū nāmā”ti?",
  "mil3.3.6:1.4_0": "“Yo, bhante,",
  "mil3.3.6:1.4_1": "abbhantare jīvo cakkhunā rūpaṁ passati,",
  "mil3.3.6:1.4_2": "sotena saddaṁ suṇāti,",
  "mil3.3.6:1.4_3": "ghānena gandhaṁ ghāyati,",
  "mil3.3.6:1.4_4": "jivhāya rasaṁ sāyati,",
  "mil3.3.6:1.4_5": "kāyena phoṭṭhabbaṁ phusati,",
  "mil3.3.6:1.4_6": "manasā dhammaṁ vijānāti,",
  "mil3.3.6:1.4_7": "yathā mayaṁ idha pāsāde nisinnā yena yena vātapānena iccheyyāma passituṁ,",
  "mil3.3.6:1.4_8": "tena tena vātapānena passeyyāma,",
  "mil3.3.6:1.4_9": "puratthimenapi vātapānena passeyyāma,",
  "mil3.3.6:1.4_10": "pacchimenapi vātapānena passeyyāma,",
  "mil3.3.6:1.4_11": "uttarenapi vātapānena passeyyāma,",
  "mil3.3.6:1.4_12": "dakkhiṇenapi vātapānena passeyyāma.",
  "mil3.3.6:1.5_0": "Evameva kho, bhante,",
  "mil3.3.6:1.5_1": "ayaṁ abbhantare jīvo yena yena dvārena icchati passituṁ,",
  "mil3.3.6:1.5_2": "tena tena dvārena passatī”ti.",
  "mil3.3.6:2.1_0": "Thero āha—",
  "mil3.3.6:2.2_0": "“pañcadvāraṁ, mahārāja,",
  "mil3.3.6:2.2_1": "bhaṇissāmi,",
  "mil3.3.6:2.2_2": "taṁ suṇohi,",
  "mil3.3.6:2.2_3": "sādhukaṁ manasikarohi,",
  "mil3.3.6:2.2_4": "yadi abbhantare jīvo cakkhunā rūpaṁ passati,",
  "mil3.3.6:2.2_5": "yathā mayaṁ idha pāsāde nisinnā yena yena vātapānena iccheyyāma passituṁ,",
  "mil3.3.6:2.2_6": "tena tena vātapānena rūpaṁyeva passeyyāma,",
  "mil3.3.6:2.2_7": "puratthimenapi vātapānena rūpaṁyeva passeyyāma,",
  "mil3.3.6:2.2_8": "pacchimenapi vātapānena rūpaṁyeva passeyyāma,",
  "mil3.3.6:2.2_9": "uttarenapi vātapānena rūpaṁyeva passeyyāma,",
  "mil3.3.6:2.2_10": "dakkhiṇenapi vātapānena rūpaṁyeva passeyyāma,",
  "mil3.3.6:2.2_11": "evametena abbhantare jīvena sotenapi rūpaṁyeva passitabbaṁ,",
  "mil3.3.6:2.2_12": "ghānenapi rūpaṁyeva passitabbaṁ,",
  "mil3.3.6:2.2_13": "jivhāyapi rūpaṁyeva passitabbaṁ,",
  "mil3.3.6:2.2_14": "kāyenapi rūpaṁyeva passitabbaṁ,",
  "mil3.3.6:2.2_15": "manasāpi rūpaṁyeva passitabbaṁ;",
  "mil3.3.6:2.3_0": "cakkhunāpi saddoyeva sotabbo,",
  "mil3.3.6:2.3_1": "ghānenapi saddoyeva sotabbo,",
  "mil3.3.6:2.3_2": "jivhāyapi saddoyeva sotabbo,",
  "mil3.3.6:2.3_3": "kāyenapi saddoyeva sotabbo,",
  "mil3.3.6:2.3_4": "manasāpi saddoyeva sotabbo;",
  "mil3.3.6:2.4_0": "cakkhunāpi gandhoyeva ghāyitabbo,",
  "mil3.3.6:2.4_1": "sotenapi gandhoyeva ghāyitabbo,",
  "mil3.3.6:2.4_2": "jivhāyapi gandhoyeva ghāyitabbo,",
  "mil3.3.6:2.4_3": "kāyenapi gandhoyeva ghāyitabbo,",
  "mil3.3.6:2.4_4": "manasāpi gandhoyeva ghāyitabbo;",
  "mil3.3.6:2.5_0": "cakkhunāpi rasoyeva sāyitabbo,",
  "mil3.3.6:2.5_1": "sotenapi rasoyeva sāyitabbo,",
  "mil3.3.6:2.5_2": "ghānenapi rasoyeva sāyitabbo,",
  "mil3.3.6:2.5_3": "kāyenapi rasoyeva sāyitabbo,",
  "mil3.3.6:2.5_4": "manasāpi rasoyeva sāyitabbo;",
  "mil3.3.6:2.6_0": "cakkhunāpi phoṭṭhabbaṁyeva phusitabbaṁ,",
  "mil3.3.6:2.6_1": "sotenapi phoṭṭhabbaṁ yeva phusitabbaṁ,",
  "mil3.3.6:2.6_2": "ghānenapi phoṭṭhabbaṁyeva phusitabbaṁ,",
  "mil3.3.6:2.6_3": "jivhāyapi phoṭṭhabbaṁ yeva phusitabbaṁ,",
  "mil3.3.6:2.6_4": "manasāpi phoṭṭhabbaṁyeva phusitabbaṁ;",
  "mil3.3.6:2.7_0": "cakkhunāpi dhammaṁ yeva vijānitabbaṁ,",
  "mil3.3.6:2.7_1": "sotenapi dhammaṁyeva vijānitabbaṁ,",
  "mil3.3.6:2.7_2": "ghānenapi dhammaṁ yeva vijānitabbaṁ,",
  "mil3.3.6:2.7_3": "jivhāyapi dhammaṁyeva vijānitabbaṁ,",
  "mil3.3.6:2.7_4": "kāyenapi dhammaṁyeva vijānitabban”ti?",
  "mil3.3.6:2.8_0": "“Na hi, bhante”ti.",
  "mil3.3.6:3.1_0": "“Na kho te, mahārāja,",
  "mil3.3.6:3.1_1": "yujjati purimena vā pacchimaṁ,",
  "mil3.3.6:3.1_2": "pacchimena vā purimaṁ,",
  "mil3.3.6:3.1_3": "yathā vā pana, mahārāja,",
  "mil3.3.6:3.1_4": "mayaṁ idha pāsāde nisinnā imesu jālavātapānesu ugghāṭitesu mahantena ākāsena bahimukhā suṭṭhutaraṁ rūpaṁ passāma,",
  "mil3.3.6:3.1_5": "evametena abbhantare jīvenāpi cakkhudvāresu ugghāṭitesu mahantena ākāsena suṭṭhutaraṁ rūpaṁ passitabbaṁ,",
  "mil3.3.6:3.1_6": "sotesu ugghāṭitesu …",
  "mil3.3.6:3.1_7": "ghāne ugghāṭite …",
  "mil3.3.6:3.1_8": "jivhāya ugghāṭitāya …",
  "mil3.3.6:3.1_9": "kāye ugghāṭite mahantena ākāsena suṭṭhutaraṁ saddo sotabbo,",
  "mil3.3.6:3.1_10": "gandho ghāyitabbo,",
  "mil3.3.6:3.1_11": "raso sāyitabbo,",
  "mil3.3.6:3.1_12": "phoṭṭhabbo phusitabbo”ti?",
  "mil3.3.6:3.2_0": "“Na hi, bhante”ti.",
  "mil3.3.6:4.1_0": "“Na kho te, mahārāja,",
  "mil3.3.6:4.1_1": "yujjati purimena vā pacchimaṁ,",
  "mil3.3.6:4.1_2": "pacchimena vā purimaṁ,",
  "mil3.3.6:4.1_3": "yathā vā pana, mahārāja,",
  "mil3.3.6:4.1_4": "ayaṁ dinno nikkhamitvā bahidvārakoṭṭhake tiṭṭheyya,",
  "mil3.3.6:4.1_5": "jānāsi tvaṁ, mahārāja,",
  "mil3.3.6:4.1_6": "‘ayaṁ dinno nikkhamitvā bahidvārakoṭṭhake ṭhito’”ti?",
  "mil3.3.6:4.2_0": "“Āma, bhante, jānāmī”ti.",
  "mil3.3.6:4.3_0": "“Yathā vā pana, mahārāja,",
  "mil3.3.6:4.3_1": "ayaṁ dinno anto pavisitvā tava purato tiṭṭheyya,",
  "mil3.3.6:4.3_2": "jānāsi tvaṁ, mahārāja,",
  "mil3.3.6:4.3_3": "‘ayaṁ dinno anto pavisitvā mama purato ṭhito’”ti?",
  "mil3.3.6:4.4_0": "“Āma, bhante, jānāmī”ti.",
  "mil3.3.6:4.5_0": "“Evameva kho, mahārāja,",
  "mil3.3.6:4.5_1": "abbhantare so jīvo jivhāya rase nikkhitte jāneyya ambilattaṁ vā lavaṇattaṁ vā tittakattaṁ vā kaṭukattaṁ vā kasāyattaṁ vā madhurattaṁ vā”ti?",
  "mil3.3.6:4.6_0": "“Āma, bhante, jāneyyā”ti.",
  "mil3.3.6:4.7_0": "“Te rase anto paviṭṭhe jāneyya ambilattaṁ vā lavaṇattaṁ vā tittakattaṁ vā kaṭukattaṁ vā kasāyattaṁ vā madhurattaṁ vā”ti.",
  "mil3.3.6:4.8_0": "“Na hi, bhante”ti.",
  "mil3.3.6:5.1_0": "“Na kho te, mahārāja,",
  "mil3.3.6:5.1_1": "yujjati purimena vā pacchimaṁ,",
  "mil3.3.6:5.1_2": "pacchimena vā purimaṁ, yathā, mahārāja,",
  "mil3.3.6:5.1_3": "kocideva puriso madhughaṭasataṁ āharāpetvā madhudoṇiṁ pūrāpetvā purisassa mukhaṁ pidahitvā madhudoṇiyā pakkhipeyya, jāneyya, mahārāja,",
  "mil3.3.6:5.1_4": "so puriso madhuṁ sampannaṁ vā na sampannaṁ vā”ti?",
  "mil3.3.6:5.2_0": "“Na hi, bhante”ti.",
  "mil3.3.6:5.3_0": "“Kena kāraṇenā”ti?",
  "mil3.3.6:5.4_0": "“Na hi tassa, bhante, mukhe madhu paviṭṭhan”ti.",
  "mil3.3.6:6.1_0": "“Na kho te, mahārāja, yujjati purimena vā pacchimaṁ, pacchimena vā puriman”ti.",
  "mil3.3.6:6.2_0": "“Nāhaṁ paṭibalo tayā vādinā saddhiṁ sallapituṁ;",
  "mil3.3.6:6.3_0": "sādhu, bhante, atthaṁ jappehī”ti.",
  "mil3.3.6:7.1_0": "Thero abhidhammasaṁyuttāya kathāya rājānaṁ milindaṁ saññāpesi—",
  "mil3.3.6:7.2_0": "“idha, mahārāja,",
  "mil3.3.6:7.2_1": "cakkhuñca paṭicca rūpe ca uppajjati cakkhuviññāṇaṁ,",
  "mil3.3.6:7.2_2": "taṁsahajātā phasso vedanā saññā cetanā ekaggatā jīvitindriyaṁ manasikāroti evamete dhammā paccayato jāyanti,",
  "mil3.3.6:7.2_3": "na hettha vedagū upalabbhati,",
  "mil3.3.6:7.2_4": "sotañca paṭicca sadde ca …pe…",
  "mil3.3.6:7.3_0": "manañca paṭicca dhamme ca uppajjati manoviññāṇaṁ,",
  "mil3.3.6:7.3_1": "taṁsahajātā phasso vedanā saññā cetanā ekaggatā jīvitindriyaṁ manasikāroti evamete dhammā paccayato jāyanti,",
  "mil3.3.6:7.3_2": "na hettha vedagū upalabbhatī”ti.",
  "mil3.3.6:8.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.3.6:9.1_0": "Vedagūpañho chaṭṭho."
}