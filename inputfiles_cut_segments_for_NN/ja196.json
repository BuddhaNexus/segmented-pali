{
  "ja196:0.1_0": "Jātaka",
  "ja196:0.2_0": "Dukanipāta",
  "ja196:0.3_0": "Ruhakavagga",
  "ja196:0.4_0": "6. Valāhakassajātaka",
  "ja196:1.1_0": "“Ye na kāhanti ovādaṁ,",
  "ja196:1.2_0": "narā buddhena desitaṁ;",
  "ja196:1.3_0": "Byasanaṁ te gamissanti,",
  "ja196:1.4_0": "rakkhasīhiva vāṇijā.",
  "ja196:2.1_0": "Ye ca kāhanti ovādaṁ,",
  "ja196:2.2_0": "narā buddhena desitaṁ;",
  "ja196:2.3_0": "Sotthiṁ pāraṁ gamissanti,",
  "ja196:2.4_0": "valāheneva vāṇijā”ti.",
  "ja196:3.1_0": "Valāhakassajātakaṁ chaṭṭhaṁ."
}