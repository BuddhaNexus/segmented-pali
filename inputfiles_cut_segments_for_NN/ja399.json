{
  "ja399:0.1_0": "Jātaka",
  "ja399:0.2_0": "Sattakanipāta",
  "ja399:0.3_0": "Kukkuvagga",
  "ja399:0.4_0": "4. Mātuposakagijjhajātaka",
  "ja399:1.1_0": "“Te kathaṁ nu karissanti,",
  "ja399:1.2_0": "vuddhā giridarīsayā;",
  "ja399:1.3_0": "Ahaṁ baddhosmi pāsena,",
  "ja399:1.4_0": "nilīyassa vasaṁ gato”.",
  "ja399:2.1_0": "“Kiṁ gijjha paridevasi,",
  "ja399:2.2_0": "kā nu te paridevanā;",
  "ja399:2.3_0": "Na me suto vā diṭṭho vā,",
  "ja399:2.4_0": "bhāsanto mānusiṁ dijo”.",
  "ja399:3.1_0": "“Bharāmi mātāpitaro,",
  "ja399:3.2_0": "vuddhe giridarīsaye;",
  "ja399:3.3_0": "Te kathaṁ nu karissanti,",
  "ja399:3.4_0": "ahaṁ vasaṁ gato tava”.",
  "ja399:4.1_0": "“Yannu gijjho yojanasataṁ,",
  "ja399:4.2_0": "kuṇapāni avekkhati;",
  "ja399:4.3_0": "Kasmā jālañca pāsañca,",
  "ja399:4.4_0": "āsajjāpi na bujjhasi”.",
  "ja399:5.1_0": "“Yadā parābhavo hoti,",
  "ja399:5.2_0": "poso jīvitasaṅkhaye;",
  "ja399:5.3_0": "Atha jālañca pāsañca,",
  "ja399:5.4_0": "āsajjāpi na bujjhati”.",
  "ja399:6.1_0": "“Bharassu mātāpitaro,",
  "ja399:6.2_0": "vuddhe giridarīsaye;",
  "ja399:6.3_0": "Mayā tvaṁ samanuññāto,",
  "ja399:6.4_0": "sotthiṁ passāhi ñātake”.",
  "ja399:7.1_0": "“Evaṁ luddaka nandassu,",
  "ja399:7.2_0": "saha sabbehi ñātibhi;",
  "ja399:7.3_0": "Bharissaṁ mātāpitaro,",
  "ja399:7.4_0": "vuddhe giridarīsaye”ti.",
  "ja399:8.1_0": "Mātuposakagijjhajātakaṁ catutthaṁ."
}