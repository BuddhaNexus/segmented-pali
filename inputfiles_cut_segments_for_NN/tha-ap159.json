{
  "tha-ap159:0.1_0": "Therāpadāna",
  "tha-ap159:0.2_0": "Bandhujīvakavagga",
  "tha-ap159:0.3_0": "7. Tiṇasūlakattheraapadāna",
  "tha-ap159:1.1_0": "“Himavantassāvidūre,",
  "tha-ap159:1.2_0": "bhūtagaṇo nāma pabbato;",
  "tha-ap159:1.3_0": "Vasateko jino tattha,",
  "tha-ap159:1.4_0": "sayambhū lokanissaṭo.",
  "tha-ap159:2.1_0": "Tiṇasūlaṁ gahetvāna,",
  "tha-ap159:2.2_0": "buddhassa abhiropayiṁ;",
  "tha-ap159:2.3_0": "Ekūnasatasahassaṁ,",
  "tha-ap159:2.4_0": "kappaṁ na vinipātiko.",
  "tha-ap159:3.1_0": "Ito ekādase kappe,",
  "tha-ap159:3.2_0": "ekosiṁ dharaṇīruho;",
  "tha-ap159:3.3_0": "Sattaratanasampanno,",
  "tha-ap159:3.4_0": "cakkavattī mahabbalo.",
  "tha-ap159:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap159:4.2_0": "…pe…",
  "tha-ap159:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap159:5.1_0": "Itthaṁ sudaṁ āyasmā tiṇasūlako thero imā gāthāyo abhāsitthāti.",
  "tha-ap159:6.1_0": "Tiṇasūlakattherassāpadānaṁ sattamaṁ."
}