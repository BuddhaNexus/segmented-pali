{
  "mil3.7.16:0.1_0": "Milindapañha",
  "mil3.7.16:0.2_0": "Arūpadhammavavatthānavagga",
  "mil3.7.16:0.3_0": "16. Arūpadhammavavatthānadukkarapañha",
  "mil3.7.16:1.1_0": "Rājā āha—",
  "mil3.7.16:1.2_0": "“bhante nāgasena, dukkaraṁ nu kho bhagavatā katan”ti?",
  "mil3.7.16:1.3_0": "Thero āha—",
  "mil3.7.16:1.4_0": "“dukkaraṁ, mahārāja, bhagavatā katan”ti.",
  "mil3.7.16:1.5_0": "“Kiṁ pana, bhante nāgasena, bhagavatā dukkaraṁ katan”ti.",
  "mil3.7.16:1.6_0": "“Dukkaraṁ, mahārāja,",
  "mil3.7.16:1.6_1": "bhagavatā kataṁ imesaṁ arūpīnaṁ cittacetasikānaṁ dhammānaṁ ekārammaṇe vattamānānaṁ vavatthānaṁ akkhātaṁ ‘ayaṁ phasso,",
  "mil3.7.16:1.6_2": "ayaṁ vedanā,",
  "mil3.7.16:1.6_3": "ayaṁ saññā,",
  "mil3.7.16:1.6_4": "ayaṁ cetanā,",
  "mil3.7.16:1.6_5": "idaṁ cittan’”ti.",
  "mil3.7.16:2.1_0": "“Opammaṁ karohī”ti.",
  "mil3.7.16:2.2_0": "“Yathā, mahārāja,",
  "mil3.7.16:2.2_1": "kocideva puriso nāvāya mahāsamuddaṁ ajjhogāhetvā hatthapuṭena udakaṁ gahetvā jivhāya sāyitvā jāneyya nu kho, mahārāja,",
  "mil3.7.16:2.2_2": "so puriso ‘idaṁ gaṅgāya udakaṁ,",
  "mil3.7.16:2.2_3": "idaṁ yamunāya udakaṁ,",
  "mil3.7.16:2.2_4": "idaṁ aciravatiyā udakaṁ,",
  "mil3.7.16:2.2_5": "idaṁ sarabhuyā udakaṁ,",
  "mil3.7.16:2.2_6": "idaṁ mahiyā udakan’”ti?",
  "mil3.7.16:2.3_0": "“Dukkaraṁ, bhante, jānitun”ti.",
  "mil3.7.16:2.4_0": "“Ito dukkarataraṁ kho, mahārāja,",
  "mil3.7.16:2.4_1": "bhagavatā kataṁ imesaṁ arūpīnaṁ cittacetasikānaṁ dhammānaṁ ekārammaṇe vattamānānaṁ vavatthānaṁ akkhātaṁ ‘ayaṁ phasso,",
  "mil3.7.16:2.4_2": "ayaṁ vedanā,",
  "mil3.7.16:2.4_3": "ayaṁ saññā,",
  "mil3.7.16:2.4_4": "ayaṁ cetanā,",
  "mil3.7.16:2.4_5": "idaṁ cittan’”ti.",
  "mil3.7.16:2.5_0": "“Suṭṭhu, bhante”ti rājā abbhānumodīti.",
  "mil3.7.16:3.1_0": "Arūpadhammavavatthānadukkarapañho soḷasamo.",
  "mil3.7.16:4.1_0": "Arūpadhammavavatthānavaggo sattamo.",
  "mil3.7.16:5.1_0": "Imasmiṁ vagge soḷasa pañhā."
}