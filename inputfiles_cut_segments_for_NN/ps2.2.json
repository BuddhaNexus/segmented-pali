{
  "ps2.2:0.1_0": "Paṭisambhidāmagga",
  "ps2.2:0.2_0": "2 Yuganaddhavagga",
  "ps2.2:0.3_0": "2.2. Saccakathā",
  "ps2.2:1.1_0": "Purimanidānaṁ.",
  "ps2.2:1.2_0": "“Cattārimāni, bhikkhave,",
  "ps2.2:1.2_1": "tathāni avitathāni anaññathāni.",
  "ps2.2:1.3_0": "Katamāni cattāri?",
  "ps2.2:1.4_0": "‘Idaṁ dukkhan’ti, bhikkhave,",
  "ps2.2:1.4_1": "tathametaṁ avitathametaṁ anaññathametaṁ;",
  "ps2.2:1.5_0": "‘ayaṁ dukkhasamudayo’ti tathametaṁ avitathametaṁ anaññathametaṁ,",
  "ps2.2:1.5_1": "‘ayaṁ dukkhanirodho’ti tathametaṁ avitathametaṁ anaññathametaṁ,",
  "ps2.2:1.5_2": "‘ayaṁ dukkhanirodhagāminī paṭipadā’ti tathametaṁ avitathametaṁ anaññathametaṁ.",
  "ps2.2:1.6_0": "Imāni kho, bhikkhave,",
  "ps2.2:1.6_1": "cattāri tathāni avitathāni anaññathāni”.",
  "ps2.2:2.0_0": "2.2.1. Paṭhamasuttantaniddesa",
  "ps2.2:2.1_0": "Kathaṁ dukkhaṁ tathaṭṭhena saccaṁ?",
  "ps2.2:2.2_0": "Cattāro dukkhassa dukkhaṭṭhā tathā avitathā anaññathā.",
  "ps2.2:2.3_0": "Dukkhassa pīḷanaṭṭho, saṅkhataṭṭho, santāpaṭṭho, vipariṇāmaṭṭho—",
  "ps2.2:2.4_0": "ime cattāro dukkhassa dukkhaṭṭhā tathā avitathā anaññathā.",
  "ps2.2:2.5_0": "Evaṁ dukkhaṁ tathaṭṭhena saccaṁ.",
  "ps2.2:3.1_0": "Kathaṁ samudayo tathaṭṭhena saccaṁ?",
  "ps2.2:3.2_0": "Cattāro samudayassa samudayaṭṭhā tathā avitathā anaññathā.",
  "ps2.2:3.3_0": "Samudayassa āyūhanaṭṭho, nidānaṭṭho, saṁyogaṭṭho palibodhaṭṭho—",
  "ps2.2:3.4_0": "ime cattāro samudayassa samudayaṭṭhā tathā avitathā anaññathā.",
  "ps2.2:3.5_0": "Evaṁ samudayo tathaṭṭhena saccaṁ.",
  "ps2.2:4.1_0": "Kathaṁ nirodho tathaṭṭhena saccaṁ?",
  "ps2.2:4.2_0": "Cattāro nirodhassa nirodhaṭṭhā tathā avitathā anaññathā.",
  "ps2.2:4.3_0": "Nirodhassa nissaraṇaṭṭho, vivekaṭṭho, asaṅkhataṭṭho, amataṭṭho—",
  "ps2.2:4.4_0": "ime cattāro nirodhassa nirodhaṭṭhā tathā avitathā anaññathā.",
  "ps2.2:4.5_0": "Evaṁ nirodho tathaṭṭhena saccaṁ.",
  "ps2.2:5.1_0": "Kathaṁ maggo tathaṭṭhena saccaṁ?",
  "ps2.2:5.2_0": "Cattāro maggassa maggaṭṭhā tathā avitathā anaññathā.",
  "ps2.2:5.3_0": "Maggassa niyyānaṭṭho, hetuṭṭho, dassanaṭṭho, ādhipateyyaṭṭho—",
  "ps2.2:5.4_0": "ime cattāro maggassa maggaṭṭhā tathā avitathā anaññathā.",
  "ps2.2:5.5_0": "Evaṁ maggo tathaṭṭhena saccaṁ.",
  "ps2.2:6.1_0": "Katihākārehi cattāri saccāni ekappaṭivedhāni?",
  "ps2.2:6.2_0": "Catūhākārehi cattāri saccāni ekappaṭivedhāni.",
  "ps2.2:6.3_0": "Tathaṭṭhena, anattaṭṭhena, saccaṭṭhena, paṭivedhaṭṭhena—",
  "ps2.2:6.4_0": "imehi catūhākārehi cattāri saccāni ekasaṅgahitāni.",
  "ps2.2:6.5_0": "Yaṁ ekasaṅgahitaṁ taṁ ekattaṁ.",
  "ps2.2:6.6_0": "Ekattaṁ ekena ñāṇena paṭivijjhatīti—",
  "ps2.2:6.7_0": "cattāri saccāni ekappaṭivedhāni.",
  "ps2.2:7.1_0": "Kathaṁ tathaṭṭhena cattāri saccāni ekappaṭivedhāni?",
  "ps2.2:7.2_0": "Catūhākārehi tathaṭṭhena cattāri saccāni ekappaṭivedhāni.",
  "ps2.2:7.3_0": "Dukkhassa dukkhaṭṭho tathaṭṭho,",
  "ps2.2:7.3_1": "samudayassa samudayaṭṭho tathaṭṭho,",
  "ps2.2:7.3_2": "nirodhassa nirodhaṭṭho tathaṭṭho, maggassa maggaṭṭho tathaṭṭho—",
  "ps2.2:7.4_0": "imehi catūhākārehi tathaṭṭhena cattāri saccāni ekasaṅgahitāni.",
  "ps2.2:7.5_0": "Yaṁ ekasaṅgahitaṁ taṁ ekattaṁ.",
  "ps2.2:7.6_0": "Ekattaṁ ekena ñāṇena paṭivijjhatīti—",
  "ps2.2:7.7_0": "cattāri saccāni ekappaṭivedhāni.",
  "ps2.2:8.1_0": "Kathaṁ anattaṭṭhena cattāri saccāni ekappaṭivedhāni?",
  "ps2.2:8.2_0": "Catūhākārehi anattaṭṭhena cattāri saccāni ekappaṭivedhāni.",
  "ps2.2:8.3_0": "Dukkhassa dukkhaṭṭho anattaṭṭho,",
  "ps2.2:8.3_1": "samudayassa samudayaṭṭho anattaṭṭho,",
  "ps2.2:8.3_2": "nirodhassa nirodhaṭṭho anattaṭṭho,",
  "ps2.2:8.3_3": "maggassa maggaṭṭho anattaṭṭho—",
  "ps2.2:8.4_0": "imehi catūhākārehi anattaṭṭhena cattāri saccāni ekasaṅgahitāni.",
  "ps2.2:8.5_0": "Yaṁ ekasaṅgahitaṁ taṁ ekattaṁ.",
  "ps2.2:8.6_0": "Ekattaṁ ekena ñāṇena paṭivijjhatīti—",
  "ps2.2:8.7_0": "cattāri saccāni ekappaṭivedhāni.",
  "ps2.2:9.1_0": "Kathaṁ saccaṭṭhena cattāri saccāni ekappaṭivedhāni?",
  "ps2.2:9.2_0": "Catūhākārehi saccaṭṭhena cattāri saccāni ekappaṭivedhāni.",
  "ps2.2:9.3_0": "Dukkhassa dukkhaṭṭho saccaṭṭho,",
  "ps2.2:9.3_1": "samudayassa samudayaṭṭho saccaṭṭho,",
  "ps2.2:9.3_2": "nirodhassa nirodhaṭṭho saccaṭṭho, maggassa maggaṭṭho saccaṭṭho—",
  "ps2.2:9.4_0": "imehi catūhākārehi saccaṭṭhena cattāri saccāni ekasaṅgahitāni.",
  "ps2.2:9.5_0": "Yaṁ ekasaṅgahitaṁ taṁ ekattaṁ.",
  "ps2.2:9.6_0": "Ekattaṁ ekena ñāṇena paṭivijjhatīti—",
  "ps2.2:9.7_0": "cattāri saccāni ekappaṭivedhāni.",
  "ps2.2:10.1_0": "Kathaṁ paṭivedhaṭṭhena cattāri saccāni ekappaṭivedhāni?",
  "ps2.2:10.2_0": "Catūhākārehi paṭivedhaṭṭhena cattāri saccāni ekappaṭivedhāni.",
  "ps2.2:10.3_0": "Dukkhassa dukkhaṭṭho paṭivedhaṭṭho,",
  "ps2.2:10.3_1": "samudayassa samudayaṭṭho paṭivedhaṭṭho nirodhassa nirodhaṭṭho paṭivedhaṭṭho,",
  "ps2.2:10.3_2": "maggassa maggaṭṭho paṭivedhaṭṭho—",
  "ps2.2:10.4_0": "imehi catūhākārehi paṭivedhaṭṭhena cattāri saccāni ekasaṅgahitāni yaṁ ekasaṅgahitaṁ taṁ ekattaṁ.",
  "ps2.2:10.5_0": "Ekattaṁ ekena ñāṇena paṭivijjhatīti—",
  "ps2.2:10.6_0": "cattāri saccāni ekappaṭivedhāni.",
  "ps2.2:11.1_0": "Katihākārehi cattāri saccāni ekappaṭivedhāni?",
  "ps2.2:11.2_0": "Yaṁ aniccaṁ taṁ dukkhaṁ, yaṁ dukkhaṁ taṁ aniccaṁ,",
  "ps2.2:11.2_1": "yaṁ aniccañca dukkhañca taṁ anattā.",
  "ps2.2:11.3_0": "Yaṁ aniccañca dukkhañca anattā ca taṁ tathaṁ.",
  "ps2.2:11.4_0": "Yaṁ aniccaṁ ca dukkhañca anattā ca tathañca taṁ saccaṁ.",
  "ps2.2:11.5_0": "Yaṁ aniccañca dukkhañca anattā ca tathañca saccañca taṁ ekasaṅgahitaṁ.",
  "ps2.2:11.6_0": "Yaṁ ekasaṅgahitaṁ taṁ ekattaṁ.",
  "ps2.2:11.7_0": "Ekattaṁ ekena ñāṇena paṭivijjhatīti—",
  "ps2.2:11.8_0": "cattāri saccāni ekappaṭivedhāni.",
  "ps2.2:12.1_0": "Katihākārehi cattāri saccāni ekappaṭivedhāni?",
  "ps2.2:12.2_0": "Navahākārehi cattāri saccāni ekappaṭivedhāni.",
  "ps2.2:12.3_0": "Tathaṭṭhena, anattaṭṭhena, saccaṭṭhena, paṭivedhaṭṭhena, abhiññaṭṭhena, pariññaṭṭhena, pahānaṭṭhena, bhāvanaṭṭhena, sacchikiriyaṭṭhena—",
  "ps2.2:12.4_0": "imehi navahākārehi cattāri saccāni ekasaṅgahitāni.",
  "ps2.2:12.5_0": "Yaṁ ekasaṅgahitaṁ taṁ ekattaṁ.",
  "ps2.2:12.6_0": "Ekattaṁ ekena ñāṇena paṭivijjhatīti—",
  "ps2.2:12.7_0": "cattāri saccāni ekappaṭivedhāni.",
  "ps2.2:13.1_0": "Kathaṁ tathaṭṭhena cattāri saccāni ekappaṭivedhāni?",
  "ps2.2:13.2_0": "Navahākārehi tathaṭṭhena cattāri saccāni ekappaṭivedhāni.",
  "ps2.2:13.3_0": "Dukkhassa dukkhaṭṭho tathaṭṭho,",
  "ps2.2:13.3_1": "samudayassa samudayaṭṭho tathaṭṭho,",
  "ps2.2:13.3_2": "nirodhassa nirodhaṭṭho tathaṭṭho,",
  "ps2.2:13.3_3": "maggassa maggaṭṭho tathaṭṭho,",
  "ps2.2:13.3_4": "abhiññāya abhiññaṭṭho tathaṭṭho,",
  "ps2.2:13.3_5": "pariññāya pariññaṭṭho tathaṭṭho,",
  "ps2.2:13.3_6": "pahānassa pahānaṭṭho tathaṭṭho,",
  "ps2.2:13.3_7": "bhāvanāya bhāvanaṭṭho tathaṭṭho,",
  "ps2.2:13.3_8": "sacchikiriyāya sacchikiriyaṭṭho tathaṭṭho—",
  "ps2.2:13.4_0": "imehi navahākārehi tathaṭṭhena cattāri saccāni ekasaṅgahitāni.",
  "ps2.2:13.5_0": "Yaṁ ekasaṅgahitaṁ taṁ ekattaṁ.",
  "ps2.2:13.6_0": "Ekattaṁ ekena ñāṇena paṭivijjhatīti—",
  "ps2.2:13.7_0": "cattāri saccāni ekappaṭivedhāni.",
  "ps2.2:14.1_0": "Kathaṁ anattaṭṭhena …",
  "ps2.2:14.2_0": "saccaṭṭhena …",
  "ps2.2:14.3_0": "paṭivedhaṭṭhena cattāri saccāni ekappaṭivedhāni?",
  "ps2.2:14.4_0": "Navahākārehi paṭivedhaṭṭhena cattāri saccāni ekappaṭivedhāni.",
  "ps2.2:14.5_0": "Dukkhassa dukkhaṭṭho paṭivedhaṭṭho,",
  "ps2.2:14.5_1": "samudayassa samudayaṭṭho paṭivedhaṭṭho,",
  "ps2.2:14.5_2": "nirodhassa nirodhaṭṭho paṭivedhaṭṭho,",
  "ps2.2:14.5_3": "maggassa maggaṭṭho paṭivedhaṭṭho,",
  "ps2.2:14.5_4": "abhiññāya abhiññaṭṭho paṭivedhaṭṭho,",
  "ps2.2:14.5_5": "pariññāya pariññaṭṭho paṭivedhaṭṭho,",
  "ps2.2:14.5_6": "pahānassa pahānaṭṭho paṭivedhaṭṭho,",
  "ps2.2:14.5_7": "bhāvanāya bhāvanaṭṭho paṭivedhaṭṭho,",
  "ps2.2:14.5_8": "sacchikiriyāya sacchikiriyaṭṭho paṭivedhaṭṭho—",
  "ps2.2:14.6_0": "imehi navahākārehi paṭivedhaṭṭhena cattāri saccāni ekasaṅgahitāni.",
  "ps2.2:14.7_0": "Yaṁ ekasaṅgahitaṁ taṁ ekattaṁ.",
  "ps2.2:14.8_0": "Ekattaṁ ekena ñāṇena paṭivijjhatīti—",
  "ps2.2:14.9_0": "cattāri saccāni ekappaṭivedhāni.",
  "ps2.2:15.1_0": "Katihākārehi cattāri saccāni ekappaṭivedhāni?",
  "ps2.2:15.2_0": "Dvādasahākārehi cattāri saccāni ekappaṭivedhāni.",
  "ps2.2:15.3_0": "Tathaṭṭhena, anattaṭṭhena, saccaṭṭhena, paṭivedhaṭṭhena, abhijānanaṭṭhena, parijānanaṭṭhena, dhammaṭṭhena, tathaṭṭhena, ñātaṭṭhena, sacchikiriyaṭṭhena, phassanaṭṭhena, abhisamayaṭṭhena—",
  "ps2.2:15.4_0": "imehi dvādasahi ākārehi cattāri saccāni ekasaṅgahitāni.",
  "ps2.2:15.5_0": "Yaṁ ekasaṅgahitaṁ taṁ ekattaṁ.",
  "ps2.2:15.6_0": "Ekattaṁ ekena ñāṇena paṭivijjhatīti—",
  "ps2.2:15.7_0": "cattāri saccāni ekappaṭivedhāni.",
  "ps2.2:16.1_0": "Kathaṁ tathaṭṭhena cattāri saccāni ekappaṭivedhāni?",
  "ps2.2:16.2_0": "Soḷasahākārehi tathaṭṭhena cattāri saccāni ekappaṭivedhāni.",
  "ps2.2:16.3_0": "Dukkhassa pīḷanaṭṭho, saṅkhataṭṭho, santāpaṭṭho, vipariṇāmaṭṭho, tathaṭṭho;",
  "ps2.2:16.4_0": "samudayassa āyūhanaṭṭho, nidānaṭṭho, saṁyogaṭṭho, palibodhaṭṭho tathaṭṭho;",
  "ps2.2:16.5_0": "nirodhassa nissaraṇaṭṭho, vivekaṭṭho, asaṅkhataṭṭho, amataṭṭho tathaṭṭho;",
  "ps2.2:16.6_0": "maggassa niyyānaṭṭho, hetuṭṭho, dassanaṭṭho, ādhipateyyaṭṭho tathaṭṭho—",
  "ps2.2:16.7_0": "imehi soḷasahi ākārehi tathaṭṭhena cattāri saccāni ekasaṅgahitāni.",
  "ps2.2:16.8_0": "Yaṁ ekasaṅgahitaṁ taṁ ekattaṁ.",
  "ps2.2:16.9_0": "Ekattaṁ ekena ñāṇena paṭivijjhatīti—",
  "ps2.2:16.10_0": "cattāri saccāni ekappaṭivedhāni.",
  "ps2.2:17.1_0": "Kathaṁ anattaṭṭhena …pe…",
  "ps2.2:17.2_0": "saccaṭṭhena …",
  "ps2.2:17.3_0": "paṭivedhaṭṭhena …",
  "ps2.2:17.4_0": "abhijānanaṭṭhena …",
  "ps2.2:17.5_0": "parijānanaṭṭhena …",
  "ps2.2:17.6_0": "dhammaṭṭhena …",
  "ps2.2:17.7_0": "tathaṭṭhena …",
  "ps2.2:17.8_0": "ñātaṭṭhena …",
  "ps2.2:17.9_0": "sacchikiriyaṭṭhena …",
  "ps2.2:17.10_0": "phassanaṭṭhena …",
  "ps2.2:17.11_0": "abhisamayaṭṭhena cattāri saccāni ekappaṭivedhāni?",
  "ps2.2:17.12_0": "Soḷasahi ākārehi abhisamayaṭṭhena cattāri saccāni ekappaṭivedhāni.",
  "ps2.2:17.13_0": "Dukkhassa pīḷanaṭṭho, saṅkhataṭṭho, santāpaṭṭho, vipariṇāmaṭṭho, abhisamayaṭṭho;",
  "ps2.2:17.14_0": "samudayassa āyūhanaṭṭho, nidānaṭṭho, saṁyogaṭṭho, palibodhaṭṭho, abhisamayaṭṭho;",
  "ps2.2:17.15_0": "nirodhassa nissaraṇaṭṭho, vivekaṭṭho, asaṅkhataṭṭho, amataṭṭho, abhisamayaṭṭho;",
  "ps2.2:17.16_0": "maggassa niyyānaṭṭho, hetuṭṭho, dassanaṭṭho, ādhipateyyaṭṭho, abhisamayaṭṭho—",
  "ps2.2:17.17_0": "imehi soḷasahi ākārehi abhisamayaṭṭhena cattāri saccāni ekasaṅgahitāni.",
  "ps2.2:17.18_0": "Yaṁ ekasaṅgahitaṁ taṁ ekattaṁ.",
  "ps2.2:17.19_0": "Ekattaṁ ekena ñāṇena paṭivijjhatīti—",
  "ps2.2:17.20_0": "cattāri saccāni ekappaṭivedhāni.",
  "ps2.2:18.1_0": "Saccānaṁ kati lakkhaṇāni?",
  "ps2.2:18.2_0": "Saccānaṁ dve lakkhaṇāni.",
  "ps2.2:18.3_0": "Saṅkhatalakkhaṇañca, asaṅkhatalakkhaṇañca—",
  "ps2.2:18.4_0": "saccānaṁ imāni dve lakkhaṇāni.",
  "ps2.2:19.1_0": "Saccānaṁ kati lakkhaṇāni?",
  "ps2.2:19.2_0": "Saccānaṁ cha lakkhaṇāni.",
  "ps2.2:19.3_0": "Saṅkhatānaṁ saccānaṁ uppādo paññāyati, vayo paññāyati,",
  "ps2.2:19.3_1": "ṭhitānaṁ aññathattaṁ paññāyati asaṅkhatassa saccassa na uppādo paññāyati, na vayo paññāyati,",
  "ps2.2:19.3_2": "na ṭhitassa aññathattaṁ paññāyati—",
  "ps2.2:19.4_0": "saccānaṁ imāni cha lakkhaṇāni.",
  "ps2.2:20.1_0": "Saccānaṁ kati lakkhaṇāni?",
  "ps2.2:20.2_0": "Saccānaṁ dvādasa lakkhaṇāni.",
  "ps2.2:20.3_0": "Dukkhasaccassa uppādo paññāyati, vayo paññāyati,",
  "ps2.2:20.3_1": "ṭhitassa aññathattaṁ paññāyati;",
  "ps2.2:20.4_0": "samudayasaccassa uppādo paññāyati, vayo paññāyati,",
  "ps2.2:20.4_1": "ṭhitassa aññathattaṁ paññāyati;",
  "ps2.2:20.5_0": "maggasaccassa uppādo paññāyati, vayo paññāyati,",
  "ps2.2:20.5_1": "ṭhitassa aññathattaṁ paññāyati;",
  "ps2.2:20.6_0": "nirodhasaccassa na uppādo paññāyati, na vayo paññāyati,",
  "ps2.2:20.6_1": "na ṭhitassa aññathattaṁ paññāyati—",
  "ps2.2:20.7_0": "saccānaṁ imāni dvādasa lakkhaṇāni.",
  "ps2.2:21.1_0": "Catunnaṁ saccānaṁ kati kusalā, kati akusalā, kati abyākatā?",
  "ps2.2:21.2_0": "Samudayasaccaṁ akusalaṁ, maggasaccaṁ kusalaṁ, nirodhasaccaṁ abyākataṁ.",
  "ps2.2:21.3_0": "Dukkhasaccaṁ siyā kusalaṁ, siyā akusalaṁ, siyā abyākataṁ.",
  "ps2.2:22.1_0": "Siyā tīṇi saccāni ekasaccena saṅgahitāni,",
  "ps2.2:22.1_1": "ekasaccaṁ tīhi saccehi saṅgahitaṁ?",
  "ps2.2:22.2_0": "Vatthuvasena pariyāyena siyāti.",
  "ps2.2:22.3_0": "Kathañca siyā?",
  "ps2.2:22.4_0": "Yaṁ dukkhasaccaṁ akusalaṁ, samudayasaccaṁ akusalaṁ—",
  "ps2.2:22.5_0": "evaṁ akusalaṭṭhena dve saccāni ekasaccena saṅgahitāni,",
  "ps2.2:22.5_1": "ekasaccaṁ dvīhi saccehi saṅgahitaṁ.",
  "ps2.2:22.6_0": "Yaṁ dukkhasaccaṁ kusalaṁ, maggasaccaṁ kusalaṁ—",
  "ps2.2:22.7_0": "evaṁ kusalaṭṭhena dve saccāni ekasaccena saṅgahitāni,",
  "ps2.2:22.7_1": "ekasaccaṁ dvīhi saccehi saṅgahitaṁ.",
  "ps2.2:22.8_0": "Yaṁ dukkhasaccaṁ abyākataṁ, nirodhasaccaṁ abyākataṁ—",
  "ps2.2:22.9_0": "evaṁ abyākataṭṭhena dve saccāni ekasaccena saṅgahitāni,",
  "ps2.2:22.9_1": "ekasaccaṁ dvīhi saccehi saṅgahitaṁ.",
  "ps2.2:22.10_0": "Evaṁ siyā tīṇi saccāni ekasaccena saṅgahitāni,",
  "ps2.2:22.10_1": "ekasaccaṁ tīhi saccehi saṅgahitaṁ vatthuvasena pariyāyenāti.",
  "ps2.2:23.0_0": "2.2.2. Dutiyasuttantapāḷi",
  "ps2.2:23.1_0": "“Pubbe me, bhikkhave,",
  "ps2.2:23.1_1": "sambodhā anabhisambuddhassa bodhisattasseva sato etadahosi—",
  "ps2.2:23.2_0": "‘ko nu kho rūpassa assādo, ko ādīnavo, kiṁ nissaraṇaṁ;",
  "ps2.2:23.3_0": "ko vedanāya assādo, ko ādīnavo, kiṁ nissaraṇaṁ;",
  "ps2.2:23.4_0": "ko saññāya assādo, ko ādīnavo, kiṁ nissaraṇaṁ;",
  "ps2.2:23.5_0": "ko saṅkhārānaṁ assādo, ko ādīnavo, kiṁ nissaraṇaṁ;",
  "ps2.2:23.6_0": "ko viññāṇassa assādo, ko ādīnavo, kiṁ nissaraṇan’ti?",
  "ps2.2:23.7_0": "Tassa mayhaṁ, bhikkhave, etadahosi—",
  "ps2.2:23.8_0": "‘yaṁ kho rūpaṁ paṭicca uppajjati sukhaṁ somanassaṁ—",
  "ps2.2:23.9_0": "ayaṁ rūpassa assādo.",
  "ps2.2:23.10_0": "Yaṁ rūpaṁ aniccaṁ, taṁ dukkhaṁ vipariṇāmadhammaṁ—",
  "ps2.2:23.11_0": "ayaṁ rūpassa ādīnavo.",
  "ps2.2:23.12_0": "Yo rūpasmiṁ chandarāgavinayo chandarāgappahānaṁ—",
  "ps2.2:23.13_0": "idaṁ rūpassa nissaraṇaṁ.",
  "ps2.2:23.14_0": "Yaṁ vedanaṁ paṭicca …pe…",
  "ps2.2:23.15_0": "yaṁ saññaṁ paṭicca …",
  "ps2.2:23.16_0": "yaṁ saṅkhāre paṭicca …",
  "ps2.2:23.17_0": "yaṁ viññāṇaṁ paṭicca uppajjati sukhaṁ somanassaṁ—",
  "ps2.2:23.18_0": "ayaṁ viññāṇassa assādo.",
  "ps2.2:23.19_0": "Yaṁ viññāṇaṁ aniccaṁ taṁ dukkhaṁ vipariṇāmadhammaṁ—",
  "ps2.2:23.20_0": "ayaṁ viññāṇassa ādīnavo.",
  "ps2.2:23.21_0": "Yo viññāṇasmiṁ chandarāgavinayo chandarāgappahānaṁ—",
  "ps2.2:23.22_0": "idaṁ viññāṇassa nissaraṇaṁ’.",
  "ps2.2:24.1_0": "Yāvakīvañcāhaṁ, bhikkhave,",
  "ps2.2:24.1_1": "imesaṁ pañcannaṁ upādānakkhandhānaṁ evaṁ assādañca assādato ādīnavañca ādīnavato nissaraṇañca nissaraṇato yathābhūtaṁ nābbhaññāsiṁ, neva tāvāhaṁ, bhikkhave,",
  "ps2.2:24.1_2": "sadevake loke samārake sabrahmake sassamaṇabrāhmaṇiyā pajāya sadevamanussāya ‘anuttaraṁ sammāsambodhiṁ abhisambuddho’ti paccaññāsiṁ yato ca khvāhaṁ, bhikkhave,",
  "ps2.2:24.1_3": "imesaṁ pañcannaṁ upādānakkhandhānaṁ evaṁ assādañca assādato ādīnavañca ādīnavato nissaraṇañca nissaraṇato yathābhūtaṁ abbhaññāsiṁ, athāhaṁ, bhikkhave,",
  "ps2.2:24.1_4": "‘sadevake loke samārake sabrahmake sassamaṇabrāhmaṇiyā pajāya sadevamanussāya anuttaraṁ sammāsambodhiṁ abhisambuddho’ti paccaññāsiṁ.",
  "ps2.2:24.2_0": "Ñāṇañca pana me dassanaṁ udapādi—",
  "ps2.2:24.3_0": "‘akuppā me vimutti.",
  "ps2.2:24.4_0": "Ayamantimā jāti, natthi dāni punabbhavo’”ti.",
  "ps2.2:25.0_0": "2.2.3. Dutiyasuttantaniddesa",
  "ps2.2:25.1_0": "Yaṁ rūpaṁ paṭicca uppajjati sukhaṁ somanassaṁ, ayaṁ rūpassa assādoti—",
  "ps2.2:25.2_0": "pahānappaṭivedho samudayasaccaṁ.",
  "ps2.2:25.3_0": "Yaṁ rūpaṁ aniccaṁ taṁ dukkhaṁ vipariṇāmadhammaṁ, ayaṁ rūpassa ādīnavoti—",
  "ps2.2:25.4_0": "pariññāpaṭivedho dukkhasaccaṁ.",
  "ps2.2:25.5_0": "Yo rūpasmiṁ chandarāgavinayo chandarāgappahānaṁ, idaṁ rūpassa nissaraṇanti—",
  "ps2.2:25.6_0": "sacchikiriyāpaṭivedho nirodhasaccaṁ.",
  "ps2.2:25.7_0": "Yā imesu tīsu ṭhānesu diṭṭhi saṅkappo vācā kammanto ājīvo vāyāmo sati samādhi—",
  "ps2.2:25.8_0": "bhāvanāpaṭivedho maggasaccaṁ.",
  "ps2.2:26.1_0": "Yaṁ vedanaṁ paṭicca …pe…",
  "ps2.2:26.2_0": "yaṁ saññaṁ paṭicca …",
  "ps2.2:26.3_0": "yaṁ saṅkhāre paṭicca …",
  "ps2.2:26.4_0": "yaṁ viññāṇaṁ paṭicca uppajjati sukhaṁ somanassaṁ, ayaṁ viññāṇassa assādoti—",
  "ps2.2:26.5_0": "pahānappaṭivedho samudayasaccaṁ.",
  "ps2.2:26.6_0": "Yaṁ viññāṇaṁ aniccaṁ taṁ dukkhaṁ vipariṇāmadhammaṁ, ayaṁ viññāṇassa ādīnavoti—",
  "ps2.2:26.7_0": "pariññāpaṭivedho dukkhasaccaṁ.",
  "ps2.2:26.8_0": "Yo viññāṇasmiṁ chandarāgavinayo chandarāgappahānaṁ, idaṁ viññāṇassa nissaraṇanti—",
  "ps2.2:26.9_0": "sacchikiriyāpaṭivedho nirodhasaccaṁ.",
  "ps2.2:26.10_0": "Yā imesu tīsu ṭhānesu diṭṭhi saṅkappo vācā kammanto ājīvo vāyāmo sati samādhi—",
  "ps2.2:26.11_0": "bhāvanāpaṭivedho maggasaccaṁ.",
  "ps2.2:27.1_0": "**Saccan**ti katihākārehi saccaṁ?",
  "ps2.2:27.2_0": "Esanaṭṭhena, pariggahaṭṭhena, paṭivedhaṭṭhena.",
  "ps2.2:27.3_0": "Kathaṁ esanaṭṭhena saccaṁ?",
  "ps2.2:27.4_0": "Jarāmaraṇaṁ kiṁnidānaṁ, kiṁsamudayaṁ, kiṁjātikaṁ, kiṁpabhavanti—",
  "ps2.2:27.5_0": "evaṁ esanaṭṭhena saccaṁ.",
  "ps2.2:27.6_0": "Jarāmaraṇaṁ jātinidānaṁ, jātisamudayaṁ, jātijātikaṁ, jātippabhavanti—",
  "ps2.2:27.7_0": "evaṁ pariggahaṭṭhena saccaṁ.",
  "ps2.2:27.8_0": "Jarāmaraṇañca pajānāti,",
  "ps2.2:27.8_1": "jarāmaraṇasamudayañca pajānāti,",
  "ps2.2:27.8_2": "jarāmaraṇanirodhañca pajānāti,",
  "ps2.2:27.8_3": "jarāmaraṇanirodhagāminiṁ paṭipadañca pajānāti—",
  "ps2.2:27.9_0": "evaṁ paṭivedhaṭṭhena saccaṁ.",
  "ps2.2:28.1_0": "Jāti kiṁnidānā, kiṁsamudayā, kiṁjātikā, kiṁpabhavāti—",
  "ps2.2:28.2_0": "evaṁ esanaṭṭhena saccaṁ.",
  "ps2.2:28.3_0": "Jāti bhavanidānā, bhavasamudayā, bhavajātikā, bhavappabhavāti—",
  "ps2.2:28.4_0": "evaṁ pariggahaṭṭhena saccaṁ.",
  "ps2.2:28.5_0": "Jātiñca pajānāti, jātisamudayañca pajānāti, jātinirodhañca pajānāti,",
  "ps2.2:28.5_1": "jātinirodhagāminiṁ paṭipadañca pajānāti—",
  "ps2.2:28.6_0": "evaṁ paṭivedhaṭṭhena saccaṁ.",
  "ps2.2:29.1_0": "Bhavo kiṁnidāno, kiṁsamudayo, kiṁjātiko, kiṁpabhavoti—",
  "ps2.2:29.2_0": "evaṁ esanaṭṭhena saccaṁ.",
  "ps2.2:29.3_0": "Bhavo upādānanidāno, upādānasamudayo, upādānajātiko, upādānappabhavoti—",
  "ps2.2:29.4_0": "evaṁ pariggahaṭṭhena saccaṁ.",
  "ps2.2:29.5_0": "Bhavaṁ ca pajānāti, bhavasamudayañca pajānāti, bhavanirodhañca pajānāti,",
  "ps2.2:29.5_1": "bhavanirodhagāminiṁ paṭipadañca pajānāti—",
  "ps2.2:29.6_0": "evaṁ paṭivedhaṭṭhena saccaṁ.",
  "ps2.2:30.1_0": "Upādānaṁ kiṁnidānaṁ, kiṁsamudayaṁ, kiṁjātikaṁ, kiṁpabhavanti—",
  "ps2.2:30.2_0": "evaṁ esanaṭṭhena saccaṁ.",
  "ps2.2:30.3_0": "Upādānaṁ taṇhānidānaṁ, taṇhāsamudayaṁ, taṇhājātikaṁ, taṇhāpabhavanti—",
  "ps2.2:30.4_0": "evaṁ pariggahaṭṭhena saccaṁ.",
  "ps2.2:30.5_0": "Upādānañca pajānāti, upādānasamudayañca pajānāti,",
  "ps2.2:30.5_1": "upādānanirodhañca pajānāti upādānanirodhagāminiṁ paṭipadañca pajānāti—",
  "ps2.2:30.6_0": "evaṁ paṭivedhaṭṭhena saccaṁ.",
  "ps2.2:31.1_0": "Taṇhā kiṁnidānā, kiṁsamudayā, kiṁjātikā, kiṁpabhavāti—",
  "ps2.2:31.2_0": "evaṁ esanaṭṭhena saccaṁ.",
  "ps2.2:31.3_0": "Taṇhā vedanānidānā, vedanāsamudayā, vedanājātikā, vedanāpabhavāti—",
  "ps2.2:31.4_0": "evaṁ pariggahaṭṭhena saccaṁ.",
  "ps2.2:31.5_0": "Taṇhañca pajānāti, taṇhāsamudayañca pajānāti, taṇhānirodhañca pajānāti,",
  "ps2.2:31.5_1": "taṇhānirodhagāminiṁ paṭipadañca pajānāti—",
  "ps2.2:31.6_0": "evaṁ paṭivedhaṭṭhena saccaṁ.",
  "ps2.2:32.1_0": "Vedanā kiṁnidānā, kiṁsamudayā, kiṁjātikā, kiṁpabhavāti—",
  "ps2.2:32.2_0": "evaṁ esanaṭṭhena saccaṁ.",
  "ps2.2:32.3_0": "Vedanā phassanidānā, phassasamudayā, phassajātikā, phassappabhavāti—",
  "ps2.2:32.4_0": "evaṁ pariggahaṭṭhena saccaṁ.",
  "ps2.2:32.5_0": "Vedanañca pajānāti, vedanāsamudayañca pajānāti, vedanānirodhañca pajānāti,",
  "ps2.2:32.5_1": "vedanānirodhagāminiṁ paṭipadañca pajānāti—",
  "ps2.2:32.6_0": "evaṁ paṭivedhaṭṭhena saccaṁ.",
  "ps2.2:33.1_0": "Phasso kiṁnidāno, kiṁsamudayo, kiṁjātiko, kiṁpabhavoti—",
  "ps2.2:33.2_0": "evaṁ esanaṭṭhena saccaṁ.",
  "ps2.2:33.3_0": "Phasso saḷāyatananidāno, saḷāyatanasamudayo, saḷāyatanajātiko, saḷāyatanappabhavoti—",
  "ps2.2:33.4_0": "evaṁ pariggahaṭṭhena saccaṁ.",
  "ps2.2:33.5_0": "Phassañca pajānāti, phassasamudayañca pajānāti, phassanirodhañca pajānāti,",
  "ps2.2:33.5_1": "phassanirodhagāminiṁ paṭipadañca pajānāti—",
  "ps2.2:33.6_0": "evaṁ paṭivedhaṭṭhena saccaṁ.",
  "ps2.2:34.1_0": "Saḷāyatanaṁ kiṁnidānaṁ, kiṁsamudayaṁ, kiṁjātikaṁ, kiṁpabhavanti—",
  "ps2.2:34.2_0": "evaṁ esanaṭṭhena saccaṁ.",
  "ps2.2:34.3_0": "Saḷāyatanaṁ nāmarūpanidānaṁ, nāmarūpasamudayaṁ, nāmarūpajātikaṁ, nāmarūpappabhavanti—",
  "ps2.2:34.4_0": "evaṁ pariggahaṭṭhena saccaṁ.",
  "ps2.2:34.5_0": "Saḷāyatanañca pajānāti,",
  "ps2.2:34.5_1": "saḷāyatanasamudayañca pajānāti,",
  "ps2.2:34.5_2": "saḷāyatananirodhañca pajānāti,",
  "ps2.2:34.5_3": "saḷāyatananirodhagāminiṁ paṭipadañca pajānāti—",
  "ps2.2:34.6_0": "evaṁ paṭivedhaṭṭhena saccaṁ.",
  "ps2.2:35.1_0": "Nāmarūpaṁ kiṁnidānaṁ, kiṁsamudayaṁ, kiṁjātikaṁ, kiṁpabhavanti—",
  "ps2.2:35.2_0": "evaṁ esanaṭṭhena saccaṁ.",
  "ps2.2:35.3_0": "Nāmarūpaṁ viññāṇanidānaṁ, viññāṇasamudayaṁ, viññāṇajātikaṁ, viññāṇappabhavanti—",
  "ps2.2:35.4_0": "evaṁ pariggahaṭṭhena saccaṁ.",
  "ps2.2:35.5_0": "Nāmarūpañca pajānāti,",
  "ps2.2:35.5_1": "nāmarūpasamudayañca pajānāti, nāmarūpanirodhañca pajānāti,",
  "ps2.2:35.5_2": "nāmarūpanirodhagāminiṁ paṭipadañca pajānāti—",
  "ps2.2:35.6_0": "evaṁ paṭivedhaṭṭhena saccaṁ.",
  "ps2.2:36.1_0": "Viññāṇaṁ kiṁnidānaṁ, kiṁsamudayaṁ, kiṁjātikaṁ, kiṁpabhavanti—",
  "ps2.2:36.2_0": "evaṁ esanaṭṭhena saccaṁ.",
  "ps2.2:36.3_0": "Viññāṇaṁ saṅkhāranidānaṁ, saṅkhārasamudayaṁ, saṅkhārajātikaṁ, saṅkhārappabhavanti—",
  "ps2.2:36.4_0": "evaṁ pariggahaṭṭhena saccaṁ.",
  "ps2.2:36.5_0": "Viññāṇañca pajānāti, viññāṇasamudayañca pajānāti, viññāṇanirodhañca pajānāti,",
  "ps2.2:36.5_1": "viññāṇanirodhagāminiṁ paṭipadañca pajānāti—",
  "ps2.2:36.6_0": "evaṁ paṭivedhaṭṭhena saccaṁ.",
  "ps2.2:37.1_0": "Saṅkhārā kiṁnidānā, kiṁsamudayā, kiṁjātikā, kiṁpabhavāti—",
  "ps2.2:37.2_0": "evaṁ esanaṭṭhena saccaṁ.",
  "ps2.2:37.3_0": "Saṅkhārā avijjānidānā, avijjāsamudayā, avijjājātikā, avijjāpabhavāti—",
  "ps2.2:37.4_0": "evaṁ pariggahaṭṭhena saccaṁ.",
  "ps2.2:37.5_0": "Saṅkhāre ca pajānāti,",
  "ps2.2:37.5_1": "saṅkhārasamudayañca pajānāti, saṅkhāranirodhañca pajānāti,",
  "ps2.2:37.5_2": "saṅkhāranirodhagāminiṁ paṭipadañca pajānāti—",
  "ps2.2:37.6_0": "evaṁ paṭivedhaṭṭhena saccaṁ.",
  "ps2.2:38.1_0": "Jarāmaraṇaṁ dukkhasaccaṁ, jāti samudayasaccaṁ,",
  "ps2.2:38.1_1": "ubhinnampi nissaraṇaṁ nirodhasaccaṁ, nirodhappajānanā maggasaccaṁ.",
  "ps2.2:38.2_0": "Jāti dukkhasaccaṁ, bhavo samudayasaccaṁ,",
  "ps2.2:38.2_1": "ubhinnampi nissaraṇaṁ nirodhasaccaṁ, nirodhappajānanā maggasaccaṁ.",
  "ps2.2:38.3_0": "Bhavo dukkhasaccaṁ, upādānaṁ samudayasaccaṁ,",
  "ps2.2:38.3_1": "ubhinnampi nissaraṇaṁ nirodhasaccaṁ, nirodhappajānanā maggasaccaṁ.",
  "ps2.2:38.4_0": "Upādānaṁ dukkhasaccaṁ, taṇhā samudayasaccaṁ,",
  "ps2.2:38.4_1": "ubhinnampi nissaraṇaṁ nirodhasaccaṁ, nirodhappajānanā maggasaccaṁ.",
  "ps2.2:38.5_0": "Taṇhā dukkhasaccaṁ, vedanā samudayasaccaṁ,",
  "ps2.2:38.5_1": "ubhinnampi nissaraṇaṁ nirodhasaccaṁ, nirodhappajānanā maggasaccaṁ.",
  "ps2.2:38.6_0": "Vedanā dukkhasaccaṁ, phasso samudayasaccaṁ,",
  "ps2.2:38.6_1": "ubhinnampi nissaraṇaṁ nirodhasaccaṁ, nirodhappajānanā maggasaccaṁ.",
  "ps2.2:38.7_0": "Phasso dukkhasaccaṁ, saḷāyatanaṁ samudayasaccaṁ,",
  "ps2.2:38.7_1": "ubhinnampi nissaraṇaṁ nirodhasaccaṁ, nirodhappajānanā maggasaccaṁ.",
  "ps2.2:38.8_0": "Saḷāyatanaṁ dukkhasaccaṁ, nāmarūpaṁ samudayasaccaṁ,",
  "ps2.2:38.8_1": "ubhinnampi nissaraṇaṁ nirodhasaccaṁ, nirodhappajānanā maggasaccaṁ.",
  "ps2.2:38.9_0": "Nāmarūpaṁ dukkhasaccaṁ, viññāṇaṁ samudayasaccaṁ,",
  "ps2.2:38.9_1": "ubhinnampi nissaraṇaṁ nirodhasaccaṁ, nirodhappajānanā maggasaccaṁ.",
  "ps2.2:38.10_0": "Viññāṇaṁ dukkhasaccaṁ, saṅkhārā samudayasaccaṁ,",
  "ps2.2:38.10_1": "ubhinnampi nissaraṇaṁ nirodhasaccaṁ, nirodhappajānanā maggasaccaṁ.",
  "ps2.2:38.11_0": "Saṅkhārā dukkhasaccaṁ, avijjā samudayasaccaṁ,",
  "ps2.2:38.11_1": "ubhinnampi nissaraṇaṁ nirodhasaccaṁ, nirodhappajānanā maggasaccaṁ.",
  "ps2.2:39.1_0": "Jarāmaraṇaṁ siyā dukkhasaccaṁ, siyā samudayasaccaṁ,",
  "ps2.2:39.1_1": "ubhinnampi nissaraṇaṁ nirodhasaccaṁ, nirodhappajānanā maggasaccaṁ.",
  "ps2.2:39.2_0": "Jāti siyā dukkhasaccaṁ, siyā samudayasaccaṁ …pe…",
  "ps2.2:39.3_0": "bhavo siyā dukkhasaccaṁ, siyā samudayasaccaṁ,",
  "ps2.2:39.3_1": "ubhinnampi nissaraṇaṁ nirodhasaccaṁ,",
  "ps2.2:39.3_2": "nirodhappajānanā maggasaccanti.",
  "ps2.2:39.4_0": "Saccakathā niṭṭhitā.",
