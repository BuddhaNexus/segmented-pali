{
  "tha-ap419:0.1_0": "Therāpadāna",
  "tha-ap419:0.2_0": "Bhaddālivagga",
  "tha-ap419:0.3_0": "7. Ucchaṅgapupphiyattheraapadāna",
  "tha-ap419:1.1_0": "“Nagare bandhumatiyā,",
  "tha-ap419:1.2_0": "ahosiṁ māliko tadā;",
  "tha-ap419:1.3_0": "Ucchaṅgaṁ pūrayitvāna,",
  "tha-ap419:1.4_0": "agamaṁ antarāpaṇaṁ.",
  "tha-ap419:2.1_0": "Bhagavā tamhi samaye,",
  "tha-ap419:2.2_0": "bhikkhusaṅghapurakkhato;",
  "tha-ap419:2.3_0": "Mahatā ānubhāvena,",
  "tha-ap419:2.4_0": "niyyāti lokanāyako.",
  "tha-ap419:3.1_0": "Disvāna lokapajjotaṁ,",
  "tha-ap419:3.2_0": "vipassiṁ lokatāraṇaṁ;",
  "tha-ap419:3.3_0": "Pupphaṁ paggayha ucchaṅgā,",
  "tha-ap419:3.4_0": "buddhaseṭṭhaṁ apūjayiṁ.",
  "tha-ap419:4.1_0": "Ekanavutito kappe,",
  "tha-ap419:4.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap419:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap419:4.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap419:5.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap419:5.2_0": "…pe…",
  "tha-ap419:5.3_0": "viharāmi anāsavo.",
  "tha-ap419:6.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap419:6.2_0": "…pe…",
  "tha-ap419:6.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap419:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap419:7.2_0": "…pe…",
  "tha-ap419:7.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap419:8.1_0": "Itthaṁ sudaṁ āyasmā ucchaṅgapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap419:9.1_0": "Ucchaṅgapupphiyattherassāpadānaṁ sattamaṁ."
}