{
  "ps2.9:0.1_0": "Paṭisambhidāmagga",
  "ps2.9:0.2_0": "Yuganaddhavagga",
  "ps2.9:0.3_0": "2.9. Balakathā",
  "ps2.9:1.1_0": "Sāvatthinidānaṁ.",
  "ps2.9:1.2_0": "“Pañcimāni, bhikkhave, balāni.",
  "ps2.9:1.3_0": "Katamāni pañca?",
  "ps2.9:1.4_0": "Saddhābalaṁ, vīriyabalaṁ, satibalaṁ, samādhibalaṁ, paññābalaṁ—",
  "ps2.9:1.5_0": "imāni kho, bhikkhave, pañca balāni.",
  "ps2.9:2.1_0": "Api ca aṭṭhasaṭṭhi balāni—",
  "ps2.9:2.2_0": "saddhābalaṁ,",
  "ps2.9:2.2_1": "vīriyabalaṁ,",
  "ps2.9:2.2_2": "satibalaṁ,",
  "ps2.9:2.2_3": "samādhibalaṁ,",
  "ps2.9:2.2_4": "paññābalaṁ,",
  "ps2.9:2.2_5": "hiribalaṁ,",
  "ps2.9:2.2_6": "ottappabalaṁ,",
  "ps2.9:2.2_7": "paṭisaṅkhānabalaṁ,",
  "ps2.9:2.2_8": "bhāvanābalaṁ,",
  "ps2.9:2.2_9": "anavajjabalaṁ,",
  "ps2.9:2.2_10": "saṅgahabalaṁ,",
  "ps2.9:2.2_11": "khantibalaṁ,",
  "ps2.9:2.2_12": "paññattibalaṁ,",
  "ps2.9:2.2_13": "nijjhattibalaṁ,",
  "ps2.9:2.2_14": "issariyabalaṁ,",
  "ps2.9:2.2_15": "adhiṭṭhānabalaṁ,",
  "ps2.9:2.2_16": "samathabalaṁ,",
  "ps2.9:2.2_17": "vipassanābalaṁ,",
  "ps2.9:2.2_18": "dasa sekhabalāni,",
  "ps2.9:2.2_19": "dasa asekhabalāni,",
  "ps2.9:2.2_20": "dasa khīṇāsavabalāni,",
  "ps2.9:2.2_21": "dasa iddhibalāni,",
  "ps2.9:2.2_22": "dasa tathāgatabalāni”.",
  "ps2.9:3.1_0": "Katamaṁ **saddhābalaṁ?**",
  "ps2.9:3.2_0": "Assaddhiye na kampatīti—saddhābalaṁ.",
  "ps2.9:3.3_0": "Sahajātānaṁ dhammānaṁ upatthambhanaṭṭhena saddhābalaṁ,",
  "ps2.9:3.3_1": "kilesānaṁ pariyādānaṭṭhena saddhābalaṁ,",
  "ps2.9:3.3_2": "paṭivedhādivisodhanaṭṭhena saddhābalaṁ,",
  "ps2.9:3.3_3": "cittassa adhiṭṭhānaṭṭhena saddhābalaṁ,",
  "ps2.9:3.3_4": "cittassa vodānaṭṭhena saddhābalaṁ,",
  "ps2.9:3.3_5": "visesādhigamaṭṭhena saddhābalaṁ,",
  "ps2.9:3.3_6": "uttaripaṭivedhaṭṭhena saddhābalaṁ,",
  "ps2.9:3.3_7": "saccābhisamayaṭṭhena saddhābalaṁ,",
  "ps2.9:3.3_8": "nirodhe patiṭṭhāpakaṭṭhena saddhābalaṁ.",
  "ps2.9:3.4_0": "Idaṁ saddhābalaṁ. ",
  "ps2.9:4.1_0": "Katamaṁ **vīriyabalaṁ?**",
  "ps2.9:4.2_0": "Kosajje na kampatīti—vīriyabalaṁ.",
  "ps2.9:4.3_0": "Sahajātānaṁ dhammānaṁ upatthambhanaṭṭhena vīriyabalaṁ,",
  "ps2.9:4.3_1": "kilesānaṁ pariyādānaṭṭhena vīriyabalaṁ,",
  "ps2.9:4.3_2": "paṭivedhādivisodhanaṭṭhena vīriyabalaṁ,",
  "ps2.9:4.3_3": "cittassa adhiṭṭhānaṭṭhena vīriyabalaṁ,",
  "ps2.9:4.3_4": "cittassa vodānaṭṭhena vīriyabalaṁ,",
  "ps2.9:4.3_5": "visesādhigamaṭṭhena vīriyabalaṁ,",
  "ps2.9:4.3_6": "uttaripaṭivedhaṭṭhena vīriyabalaṁ,",
  "ps2.9:4.3_7": "saccābhisamayaṭṭhena vīriyabalaṁ,",
  "ps2.9:4.3_8": "nirodhe patiṭṭhāpakaṭṭhena vīriyabalaṁ.",
  "ps2.9:4.4_0": "Idaṁ vīriyabalaṁ. ",
  "ps2.9:5.1_0": "Katamaṁ **satibalaṁ?**",
  "ps2.9:5.2_0": "Pamāde na kampatīti—satibalaṁ.",
  "ps2.9:5.3_0": "Sahajātānaṁ dhammānaṁ upatthambhanaṭṭhena satibalaṁ …pe…",
  "ps2.9:5.4_0": "nirodhe patiṭṭhāpakaṭṭhena satibalaṁ.",
  "ps2.9:5.5_0": "Idaṁ satibalaṁ. ",
  "ps2.9:6.1_0": "Katamaṁ **samādhibalaṁ?**",
  "ps2.9:6.2_0": "Uddhacce na kampatīti—samādhibalaṁ.",
  "ps2.9:6.3_0": "Sahajātānaṁ dhammānaṁ upatthambhanaṭṭhena samādhibalaṁ …pe…",
  "ps2.9:6.4_0": "nirodhe patiṭṭhāpakaṭṭhena samādhibalaṁ.",
  "ps2.9:6.5_0": "Idaṁ samādhibalaṁ. ",
  "ps2.9:7.1_0": "Katamaṁ **paññābalaṁ?**",
  "ps2.9:7.2_0": "Avijjāya na kampatīti—paññābalaṁ.",
  "ps2.9:7.3_0": "Sahajātānaṁ dhammānaṁ upatthambhanaṭṭhena paññābalaṁ …pe…",
  "ps2.9:7.4_0": "nirodhe patiṭṭhāpakaṭṭhena paññābalaṁ.",
  "ps2.9:7.5_0": "Idaṁ paññābalaṁ. ",
  "ps2.9:8.1_0": "Katamaṁ **hiribalaṁ?**",
  "ps2.9:8.2_0": "Nekkhammena kāmacchandaṁ hirīyatīti—",
  "ps2.9:8.3_0": "hiribalaṁ.",
  "ps2.9:8.4_0": "Abyāpādena byāpādaṁ hirīyatīti—hiribalaṁ.",
  "ps2.9:8.5_0": "Ālokasaññāya thinamiddhaṁ hirīyatīti—hiribalaṁ.",
  "ps2.9:8.6_0": "Avikkhepena uddhaccaṁ hirīyatīti—hiribalaṁ.",
  "ps2.9:8.7_0": "Dhammavavatthānena vicikicchaṁ hirīyatīti—hiribalaṁ.",
  "ps2.9:8.8_0": "Ñāṇena avijjaṁ hirīyatīti—hiribalaṁ.",
  "ps2.9:8.9_0": "Pāmojjena aratiṁ hirīyatīti—hiribalaṁ.",
  "ps2.9:8.10_0": "Paṭhamena jhānena nīvaraṇe hirīyatīti—hiribalaṁ …pe…",
  "ps2.9:8.11_0": "arahattamaggena sabbakilese hirīyatīti—hiribalaṁ.",
  "ps2.9:8.12_0": "Idaṁ hiribalaṁ. ",
  "ps2.9:9.1_0": "Katamaṁ **ottappabalaṁ?**",
  "ps2.9:9.2_0": "Nekkhammena kāmacchandaṁ ottappatīti—ottappabalaṁ.",
  "ps2.9:9.3_0": "Abyāpādena byāpādaṁ ottappatīti—ottappabalaṁ.",
  "ps2.9:9.4_0": "Ālokasaññāya thinamiddhaṁ ottappatīti— ottappabalaṁ.",
  "ps2.9:9.5_0": "Avikkhepena uddhaccaṁ ottappatīti—ottappabalaṁ.",
  "ps2.9:9.6_0": "Dhammavavatthānena vicikicchaṁ ottappatīti—ottappabalaṁ.",
  "ps2.9:9.7_0": "Ñāṇena avijjaṁ ottappatīti—ottappabalaṁ.",
  "ps2.9:9.8_0": "Pāmojjena aratiṁ ottappatīti—ottappabalaṁ.",
  "ps2.9:9.9_0": "Paṭhamena jhānena nīvaraṇe ottappatīti—ottappabalaṁ …pe…",
  "ps2.9:9.10_0": "arahattamaggena sabbakilese ottappatīti—ottappabalaṁ.",
  "ps2.9:9.11_0": "Idaṁ ottappabalaṁ. ",
  "ps2.9:10.1_0": "Katamaṁ **paṭisaṅkhānabalaṁ?**",
  "ps2.9:10.2_0": "Nekkhammena kāmacchandaṁ paṭisaṅkhātīti—paṭisaṅkhānabalaṁ.",
  "ps2.9:10.3_0": "Abyāpādena byāpādaṁ paṭisaṅkhātīti—paṭisaṅkhānabalaṁ.",
  "ps2.9:10.4_0": "Ālokasaññāya thinamiddhaṁ paṭisaṅkhātīti—paṭisaṅkhānabalaṁ.",
  "ps2.9:10.5_0": "Avikkhepena uddhaccaṁ paṭisaṅkhātīti— paṭisaṅkhānabalaṁ.",
  "ps2.9:10.6_0": "Dhammavavatthānena vicikicchaṁ paṭisaṅkhātīti— paṭisaṅkhānabalaṁ.",
  "ps2.9:10.7_0": "Ñāṇena avijjaṁ paṭisaṅkhātīti—paṭisaṅkhānabalaṁ.",
  "ps2.9:10.8_0": "Pāmojjena aratiṁ paṭisaṅkhātīti—paṭisaṅkhānabalaṁ.",
  "ps2.9:10.9_0": "Paṭhamena jhānena nīvaraṇe paṭisaṅkhātīti—paṭisaṅkhānabalaṁ …pe…",
  "ps2.9:10.10_0": "arahattamaggena sabbakilese paṭisaṅkhātīti—paṭisaṅkhānabalaṁ.",
  "ps2.9:10.11_0": "Idaṁ paṭisaṅkhānabalaṁ. ",
  "ps2.9:11.1_0": "Katamaṁ **bhāvanābalaṁ?**",
  "ps2.9:11.2_0": "Kāmacchandaṁ pajahanto nekkhammaṁ bhāvetīti—bhāvanābalaṁ.",
  "ps2.9:11.3_0": "Byāpādaṁ pajahanto abyāpādaṁ bhāvetīti—bhāvanābalaṁ.",
  "ps2.9:11.4_0": "Thinamiddhaṁ pajahanto ālokasaññaṁ bhāvetīti—bhāvanābalaṁ.",
  "ps2.9:11.5_0": "Uddhaccaṁ pajahanto avikkhepaṁ bhāvetīti— bhāvanābalaṁ.",
  "ps2.9:11.6_0": "Vicikicchaṁ pajahanto dhammavavatthānaṁ bhāvetīti—bhāvanābalaṁ.",
  "ps2.9:11.7_0": "Avijjaṁ pajahanto ñāṇaṁ bhāvetīti—bhāvanābalaṁ.",
  "ps2.9:11.8_0": "Aratiṁ pajahanto pāmojjaṁ bhāvetīti—bhāvanābalaṁ.",
  "ps2.9:11.9_0": "Nīvaraṇe pajahanto paṭhamaṁ jhānaṁ bhāvetīti— bhāvanābalaṁ …pe…",
  "ps2.9:11.10_0": "sabbakilese pajahanto arahattamaggaṁ bhāvetīti— bhāvanābalaṁ.",
  "ps2.9:11.11_0": "Idaṁ bhāvanābalaṁ. ",
  "ps2.9:12.1_0": "Katamaṁ **anavajjabalaṁ?**",
  "ps2.9:12.2_0": "Kāmacchandassa pahīnattā nekkhamme natthi kiñci vajjanti—anavajjabalaṁ.",
  "ps2.9:12.3_0": "Byāpādassa pahīnattā abyāpāde natthi kiñci vajjanti—anavajjabalaṁ.",
  "ps2.9:12.4_0": "Thinamiddhassa pahīnattā ālokasaññāya natthi kiñci vajjanti—anavajjabalaṁ.",
  "ps2.9:12.5_0": "Uddhaccassa pahīnattā avikkhepe natthi kiñci vajjanti—anavajjabalaṁ.",
  "ps2.9:12.6_0": "Vicikicchāya pahīnattā dhammavavatthāne natthi kiñci vajjanti—anavajjabalaṁ.",
  "ps2.9:12.7_0": "Avijjāya pahīnattā ñāṇe natthi kiñci vajjanti—anavajjabalaṁ.",
  "ps2.9:12.8_0": "Aratiyā pahīnattā pāmojje natthi kiñci vajjanti anavajjabalaṁ.",
  "ps2.9:12.9_0": "Nīvaraṇānaṁ pahīnattā paṭhamajjhāne natthi kiñci vajjanti—anavajjabalaṁ …pe…",
  "ps2.9:12.10_0": "sabbakilesānaṁ pahīnattā arahattamagge natthi kiñci vajjanti—anavajjabalaṁ.",
  "ps2.9:12.11_0": "Idaṁ anavajjabalaṁ. ",
  "ps2.9:13.1_0": "Katamaṁ **saṅgahabalaṁ?**",
  "ps2.9:13.2_0": "Kāmacchandaṁ pajahanto nekkhammavasena cittaṁ saṅgaṇhātīti—saṅgahabalaṁ.",
  "ps2.9:13.3_0": "Byāpādaṁ pajahanto abyāpādavasena cittaṁ saṅgaṇhātīti—saṅgahabalaṁ.",
  "ps2.9:13.4_0": "Thinamiddhaṁ pajahanto ālokasaññāvasena cittaṁ saṅgaṇhātīti—saṅgahabalaṁ …pe…",
  "ps2.9:13.5_0": "sabbakilese pajahanto arahattamaggavasena cittaṁ saṅgaṇhātīti—saṅgahabalaṁ.",
  "ps2.9:13.6_0": "Idaṁ saṅgahabalaṁ. ",
  "ps2.9:14.1_0": "Katamaṁ **khantibalaṁ?**",
  "ps2.9:14.2_0": "Kāmacchandassa pahīnattā nekkhammaṁ khamatīti—khantibalaṁ.",
  "ps2.9:14.3_0": "Byāpādassa pahīnattā abyāpādo khamatīti—khantibalaṁ.",
  "ps2.9:14.4_0": "Thinamiddhassa pahīnattā ālokasaññā khamatīti—khantibalaṁ.",
  "ps2.9:14.5_0": "Uddhaccassa pahīnattā avikkhepo khamatīti— khantibalaṁ.",
  "ps2.9:14.6_0": "Vicikicchāya pahīnattā dhammavavatthānaṁ khamatīti—khantibalaṁ.",
  "ps2.9:14.7_0": "Avijjāya pahīnattā ñāṇaṁ khamatīti—khantibalaṁ.",
  "ps2.9:14.8_0": "Aratiyā pahīnattā pāmojjaṁ khamatīti—khantibalaṁ.",
  "ps2.9:14.9_0": "Nīvaraṇānaṁ pahīnattā paṭhamaṁ jhānaṁ khamatīti— khantibalaṁ …pe…",
  "ps2.9:14.10_0": "sabbakilesānaṁ pahīnattā arahattamaggo khamatīti— khantibalaṁ.",
  "ps2.9:14.11_0": "Idaṁ khantibalaṁ. ",
  "ps2.9:15.1_0": "Katamaṁ **paññattibalaṁ?**",
  "ps2.9:15.2_0": "Kāmacchandaṁ pajahanto nekkhammavasena cittaṁ paññāpetīti—",
  "ps2.9:15.3_0": "paññattibalaṁ.",
  "ps2.9:15.4_0": "Byāpādaṁ pajahanto abyāpādavasena cittaṁ paññāpetīti—paññattibalaṁ.",
  "ps2.9:15.5_0": "Thinamiddhaṁ pajahanto ālokasaññāvasena cittaṁ paññāpetīti—paññattibalaṁ …pe…",
  "ps2.9:15.6_0": "sabbakilese pajahanto arahattamaggavasena cittaṁ paññāpetīti—paññattibalaṁ.",
  "ps2.9:15.7_0": "Idaṁ paññattibalaṁ. ",
  "ps2.9:16.1_0": "Katamaṁ **nijjhattibalaṁ?**",
  "ps2.9:16.2_0": "Kāmacchandaṁ pajahanto nekkhammavasena cittaṁ nijjhāpetīti—nijjhattibalaṁ.",
  "ps2.9:16.3_0": "Byāpādaṁ pajahanto abyāpādavasena cittaṁ nijjhāpetīti—nijjhattibalaṁ.",
  "ps2.9:16.4_0": "Thinamiddhaṁ pajahanto ālokasaññāvasena cittaṁ nijjhāpetīti— nijjhattibalaṁ …pe…",
  "ps2.9:16.5_0": "sabbakilese pajahanto arahattamaggavasena cittaṁ nijjhāpetīti—nijjhattibalaṁ.",
  "ps2.9:16.6_0": "Idaṁ nijjhattibalaṁ. ",
  "ps2.9:17.1_0": "Katamaṁ **issariyabalaṁ?**",
  "ps2.9:17.2_0": "Kāmacchandaṁ pajahanto nekkhammavasena cittaṁ vasaṁ vattetīti—issariyabalaṁ.",
  "ps2.9:17.3_0": "Byāpādaṁ pajahanto abyāpādavasena cittaṁ vasaṁ vattetīti—issariyabalaṁ.",
  "ps2.9:17.4_0": "Thinamiddhaṁ pajahanto ālokasaññāvasena cittaṁ vasaṁ vattetīti— issariyabalaṁ …pe…",
  "ps2.9:17.5_0": "sabbakilese pajahanto arahattamaggavasena cittaṁ vasaṁ vattetīti—issariyabalaṁ.",
  "ps2.9:17.6_0": "Idaṁ issariyabalaṁ. ",
  "ps2.9:18.1_0": "Katamaṁ **adhiṭṭhānabalaṁ?**",
  "ps2.9:18.2_0": "Kāmacchandaṁ pajahanto nekkhammavasena cittaṁ adhiṭṭhātīti—adhiṭṭhānabalaṁ.",
  "ps2.9:18.3_0": "Byāpādaṁ pajahanto abyāpādavasena cittaṁ adhiṭṭhātīti—adhiṭṭhānabalaṁ.",
  "ps2.9:18.4_0": "Thinamiddhaṁ pajahanto ālokasaññāvasena cittaṁ adhiṭṭhātīti— adhiṭṭhānabalaṁ …pe…",
  "ps2.9:18.5_0": "sabbakilese pajahanto arahattamaggavasena cittaṁ adhiṭṭhātīti—adhiṭṭhānabalaṁ.",
  "ps2.9:18.6_0": "Idaṁ adhiṭṭhānabalaṁ. ",
  "ps2.9:19.1_0": "Katamaṁ **samathabalaṁ?**",
  "ps2.9:19.2_0": "Nekkhammavasena cittassa ekaggatā avikkhepo samathabalaṁ,",
  "ps2.9:19.2_1": "abyāpādavasena cittassa ekaggatā avikkhepo samathabalaṁ,",
  "ps2.9:19.2_2": "ālokasaññāvasena cittassa ekaggatā avikkhepo samathabalaṁ …pe…",
  "ps2.9:19.3_0": "paṭinissaggānupassī assāsavasena cittassa ekaggatā avikkhepo samathabalaṁ,",
  "ps2.9:19.3_1": "paṭinissaggānupassī passāsavasena cittassa ekaggatā avikkhepo samathabalaṁ.",
  "ps2.9:20.1_0": "**Samathabalan**ti kenaṭṭhena samathabalaṁ?",
  "ps2.9:20.2_0": "Paṭhamena jhānena nīvaraṇe na kampatīti— samathabalaṁ.",
  "ps2.9:20.3_0": "Dutiyena jhānena vitakkavicāre na kampatīti—samathabalaṁ.",
  "ps2.9:20.4_0": "Tatiyena jhānena pītiyā na kampatīti—samathabalaṁ.",
  "ps2.9:20.5_0": "Catutthena jhānena sukhadukkhe na kampatīti—samathabalaṁ.",
  "ps2.9:20.6_0": "Ākāsānañcāyatanasamāpattiyā rūpasaññāya paṭighasaññāya nānattasaññāya na kampatīti—samathabalaṁ.",
  "ps2.9:20.7_0": "Viññāṇañcāyatanasamāpattiyā ākāsānañcāyatanasaññāya na kampatīti— samathabalaṁ.",
  "ps2.9:20.8_0": "Ākiñcaññāyatanasamāpattiyā viññāṇañcāyatanasaññāya na kampatīti— samathabalaṁ.",
  "ps2.9:20.9_0": "Nevasaññānāsaññāyatanasamāpattiyā ākiñcaññāyatanasaññāya na kampatīti—samathabalaṁ.",
  "ps2.9:20.10_0": "Uddhacce ca uddhaccasahagatakilese ca khandhe ca na kampati na calati na vedhatīti—samathabalaṁ.",
  "ps2.9:20.11_0": "Idaṁ samathabalaṁ. ",
  "ps2.9:21.1_0": "Katamaṁ **vipassanābalaṁ**?",
  "ps2.9:21.2_0": "Aniccānupassanā vipassanābalaṁ, dukkhānupassanā vipassanābalaṁ …pe…",
  "ps2.9:21.3_0": "paṭinissaggānupassanā vipassanābalaṁ,",
  "ps2.9:21.3_1": "rūpe aniccānupassanā vipassanābalaṁ,",
  "ps2.9:21.3_2": "rūpe dukkhānupassanā vipassanābalaṁ …pe…",
  "ps2.9:21.4_0": "rūpe paṭinissaggānupassanā vipassanābalaṁ, vedanāya …pe…",
  "ps2.9:21.5_0": "saññāya …",
  "ps2.9:21.6_0": "saṅkhāresu …",
  "ps2.9:21.7_0": "viññāṇe …",
  "ps2.9:21.8_0": "cakkhusmiṁ …pe…",
  "ps2.9:21.9_0": "jarāmaraṇe aniccānupassanā vipassanābalaṁ, jarāmaraṇe dukkhānupassanā vipassanābalaṁ …pe…",
  "ps2.9:21.10_0": "jarāmaraṇe paṭinissaggānupassanā vipassanābalaṁ.",
  "ps2.9:21.11_0": "**Vipassanābalan**ti kenaṭṭhena vipassanābalaṁ?",
  "ps2.9:21.12_0": "Aniccānupassanāya niccasaññāya na kampatīti— vipassanābalaṁ.",
  "ps2.9:21.13_0": "Dukkhānupassanāya sukhasaññāya na kampatīti—vipassanābalaṁ.",
  "ps2.9:21.14_0": "Anattānupassanāya attasaññāya kampatīti—vipassanābalaṁ.",
  "ps2.9:21.15_0": "Nibbidānupassanāya nandiyā na kampatīti—vipassanābalaṁ.",
  "ps2.9:21.16_0": "Virāgānupassanāya rāge na kampatīti— vipassanābalaṁ.",
  "ps2.9:21.17_0": "Nirodhānupassanāya samudaye na kampatīti—vipassanābalaṁ.",
  "ps2.9:21.18_0": "Paṭinissaggānupassanāya ādāne na kampatīti—vipassanābalaṁ.",
  "ps2.9:21.19_0": "Avijjāya ca avijjāsahagatakilese ca khandhe ca na kampati na calati na vedhatīti— vipassanābalaṁ.",
  "ps2.9:21.20_0": "Idaṁ vipassanābalaṁ. ",
  "ps2.9:22.1_0": "Katamāni **dasa sekhabalāni, dasa asekhabalāni?**",
  "ps2.9:22.2_0": "Sammādiṭṭhiṁ sikkhatīti—sekhabalaṁ. ",
  "ps2.9:22.3_0": "Tattha sikkhitattā asekhabalaṁ. ",
  "ps2.9:22.4_0": "Sammāsaṅkappaṁ sikkhatīti—sekhabalaṁ. ",
  "ps2.9:22.5_0": "Tattha sikkhitattā—",
  "ps2.9:22.6_0": "asekhabalaṁ. ",
  "ps2.9:22.7_0": "Sammāvācaṁ …pe… ",
  "ps2.9:22.8_0": "sammākammantaṁ … ",
  "ps2.9:22.9_0": "sammāājīvaṁ …",
  "ps2.9:22.10_0": "sammāvāyāmaṁ …",
  "ps2.9:22.11_0": "sammāsatiṁ …",
  "ps2.9:22.12_0": "sammāsamādhiṁ …",
  "ps2.9:22.13_0": "sammāñāṇaṁ …pe…",
  "ps2.9:22.14_0": "sammāvimuttiṁ sikkhatīti—sekhabalaṁ. ",
  "ps2.9:22.15_0": "Tattha sikkhitattā—",
  "ps2.9:22.16_0": "asekhabalaṁ. ",
  "ps2.9:22.17_0": "Imāni dasa sekhabalāni, dasa asekhabalāni. ",
  "ps2.9:23.1_0": "Katamāni **dasa khīṇāsavabalāni?**",
  "ps2.9:23.2_0": "Idha khīṇāsavassa bhikkhuno aniccato sabbe saṅkhārā yathābhūtaṁ sammappaññāya sudiṭṭhā honti.",
  "ps2.9:23.3_0": "Yampi khīṇāsavassa bhikkhuno aniccato sabbe saṅkhārā yathābhūtaṁ sammappaññāya sudiṭṭhā honti,",
  "ps2.9:23.3_1": "idampi khīṇāsavassa bhikkhuno balaṁ hoti,",
  "ps2.9:23.3_2": "yaṁ balaṁ āgamma khīṇāsavo bhikkhu āsavānaṁ khayaṁ paṭijānāti—“khīṇā me āsavā”ti.",
  "ps2.9:24.1_0": "Puna caparaṁ khīṇāsavassa bhikkhuno aṅgārakāsūpamā kāmā yathābhūtaṁ sammappaññāya sudiṭṭhā honti.",
  "ps2.9:24.2_0": "Yampi khīṇāsavassa bhikkhuno aṅgārakāsūpamā kāmā yathābhūtaṁ sammappaññāya sudiṭṭhā honti,",
  "ps2.9:24.2_1": "idampi khīṇāsavassa bhikkhuno balaṁ hoti,",
  "ps2.9:24.2_2": "yaṁ balaṁ āgamma khīṇāsavo bhikkhu āsavānaṁ khayaṁ paṭijānāti—“khīṇā me āsavā”ti.",
  "ps2.9:25.1_0": "Puna caparaṁ khīṇāsavassa bhikkhuno vivekaninnaṁ cittaṁ hoti vivekapoṇaṁ vivekapabbhāraṁ vivekaṭṭhaṁ",
  "ps2.9:25.1_1": "nekkhammābhirataṁ byantībhūtaṁ sabbaso āsavaṭṭhāniyehi dhammehi.",
  "ps2.9:25.2_0": "Yampi khīṇāsavassa bhikkhuno vivekaninnaṁ cittaṁ hoti vivekapoṇaṁ vivekapabbhāraṁ vivekaṭṭhaṁ nekkhammābhirataṁ",
  "ps2.9:25.2_1": "byantībhūtaṁ sabbaso āsavaṭṭhāniyehi dhammehi,",
  "ps2.9:25.2_2": "idampi khīṇāsavassa bhikkhuno balaṁ hoti,",
  "ps2.9:25.2_3": "yaṁ balaṁ āgamma khīṇāsavo bhikkhu āsavānaṁ khayaṁ paṭijānāti—“khīṇā me āsavā”ti.",
  "ps2.9:26.1_0": "Puna caparaṁ khīṇāsavassa bhikkhuno cattāro satipaṭṭhānā bhāvitā honti subhāvitā.",
  "ps2.9:26.2_0": "Yampi khīṇāsavassa bhikkhuno cattāro satipaṭṭhānā bhāvitā honti subhāvitā,",
  "ps2.9:26.2_1": "idampi khīṇāsavassa bhikkhuno balaṁ hoti,",
  "ps2.9:26.2_2": "yaṁ balaṁ āgamma khīṇāsavo bhikkhu āsavānaṁ khayaṁ paṭijānāti—“khīṇā me āsavā”ti.",
  "ps2.9:27.1_0": "Puna caparaṁ khīṇāsavassa bhikkhuno cattāro sammappadhānā bhāvitā honti subhāvitā …pe… ",
  "ps2.9:27.2_0": "cattāro iddhipādā bhāvitā honti subhāvitā … ",
  "ps2.9:27.3_0": "pañcindriyāni bhāvitāni honti subhāvitāni … ",
  "ps2.9:27.4_0": "pañca balāni bhāvitāni honti subhāvitāni … ",
  "ps2.9:27.5_0": "satta bojjhaṅgā bhāvitā honti subhāvitā …pe… ",
  "ps2.9:27.6_0": "ariyo aṭṭhaṅgiko maggo bhāvito hoti subhāvito.",
  "ps2.9:27.7_0": "Yampi khīṇāsavassa bhikkhuno aṭṭhaṅgiko maggo bhāvito hoti subhāvito,",
  "ps2.9:27.7_1": "idampi khīṇāsavassa bhikkhuno balaṁ hoti,",
  "ps2.9:27.7_2": "yaṁ balaṁ āgamma khīṇāsavo bhikkhu āsavānaṁ khayaṁ paṭijānāti—“khīṇā me āsavā”ti.",
  "ps2.9:27.8_0": "Imāni dasa khīṇāsavabalāni. ",
  "ps2.9:28.1_0": "Katamāni **dasa iddhibalāni?**",
  "ps2.9:28.2_0": "Adhiṭṭhānā iddhi,",
  "ps2.9:28.2_1": "vikubbanā iddhi,",
  "ps2.9:28.2_2": "manomayā iddhi,",
  "ps2.9:28.2_3": "ñāṇavipphārā iddhi,",
  "ps2.9:28.2_4": "samādhivipphārā iddhi,",
  "ps2.9:28.2_5": "ariyā iddhi,",
  "ps2.9:28.2_6": "kammavipākajā iddhi,",
  "ps2.9:28.2_7": "puññavato iddhi,",
  "ps2.9:28.2_8": "vijjāmayā iddhi,",
  "ps2.9:28.2_9": "tattha tattha sammā payogappaccayā ijjhanaṭṭhena iddhi—",
  "ps2.9:28.3_0": "imāni dasa iddhibalāni. ",
  "ps2.9:29.1_0": "Katamāni **dasa tathāgatabalāni?**",
  "ps2.9:29.2_0": "Idha tathāgato ṭhānañca ṭhānato aṭṭhānañca aṭṭhānato yathābhūtaṁ pajānāti.",
  "ps2.9:29.3_0": "Yampi tathāgato ṭhānañca ṭhānato aṭṭhānañca aṭṭhānato yathābhūtaṁ pajānāti,",
  "ps2.9:29.3_1": "idampi tathāgatassa tathāgatabalaṁ hoti,",
  "ps2.9:29.3_2": "yaṁ balaṁ āgamma tathāgato āsabhaṁ ṭhānaṁ paṭijānāti parisāsu sīhanādaṁ nadati,",
  "ps2.9:29.3_3": "brahmacakkaṁ pavatteti.",
  "ps2.9:30.1_0": "Puna caparaṁ tathāgato atītānāgatapaccuppannānaṁ kammasamādānānaṁ ṭhānaso hetuso vipākaṁ yathābhūtaṁ pajānāti.",
  "ps2.9:30.2_0": "Yampi tathāgato atītānāgatapaccuppannānaṁ kammasamādānānaṁ ṭhānaso hetuso vipākaṁ yathābhūtaṁ pajānāti,",
  "ps2.9:30.2_1": "idampi tathāgatassa tathāgatabalaṁ hoti,",
  "ps2.9:30.2_2": "yaṁ balaṁ āgamma tathāgato āsabhaṁ ṭhānaṁ paṭijānāti,",
  "ps2.9:30.2_3": "parisāsu sīhanādaṁ, nadati,",
  "ps2.9:30.2_4": "brahmacakkaṁ pavatteti.",
  "ps2.9:31.1_0": "Puna caparaṁ tathāgato sabbatthagāminiṁ paṭipadaṁ yathābhūtaṁ pajānāti.",
  "ps2.9:31.2_0": "Yampi tathāgato sabbatthagāminiṁ paṭipadaṁ yathābhūtaṁ pajānāti,",
  "ps2.9:31.2_1": "idampi tathāgatassa tathāgatabalaṁ hoti,",
  "ps2.9:31.2_2": "yaṁ balaṁ āgamma tathāgato āsabhaṁ ṭhānaṁ paṭijānāti,",
  "ps2.9:31.2_3": "parisāsu sīhanādaṁ nadati,",
  "ps2.9:31.2_4": "brahmacakkaṁ pavatteti.",
  "ps2.9:32.1_0": "Puna caparaṁ tathāgato anekadhātuṁ nānādhātuṁ lokaṁ yathābhūtaṁ pajānāti.",
  "ps2.9:32.2_0": "Yampi tathāgato anekadhātuṁ nānādhātuṁ lokaṁ yathābhūtaṁ pajānāti, idampi tathāgatassa …pe…. ",
  "ps2.9:33.1_0": "Puna caparaṁ tathāgato sattānaṁ nānādhimuttikataṁ yathābhūtaṁ pajānāti.",
  "ps2.9:33.2_0": "Yampi tathāgato sattānaṁ nānādhimuttikataṁ yathābhūtaṁ pajānāti, idampi tathāgatassa …pe…. ",
  "ps2.9:34.1_0": "Puna caparaṁ tathāgato parasattānaṁ parapuggalānaṁ indriyaparopariyattaṁ yathābhūtaṁ pajānāti.",
  "ps2.9:34.2_0": "Yampi tathāgato parasattānaṁ parapuggalānaṁ indriyaparopariyattaṁ yathābhūtaṁ pajānāti,",
  "ps2.9:34.2_1": "idampi tathāgatassa …pe….",
  "ps2.9:35.1_0": "Puna caparaṁ tathāgato jhānavimokkhasamādhisamāpattīnaṁ saṅkilesaṁ vodānaṁ vuṭṭhānaṁ yathābhūtaṁ pajānāti.",
  "ps2.9:35.2_0": "Yampi tathāgato jhānavimokkhasamādhisamāpattīnaṁ saṅkilesaṁ vodānaṁ vuṭṭhānaṁ yathābhūtaṁ pajānāti,",
  "ps2.9:35.2_1": "idampi tathāgatassa …pe….",
  "ps2.9:36.1_0": "Puna caparaṁ tathāgato anekavihitaṁ pubbenivāsaṁ anussarati, seyyathidaṁ—",
  "ps2.9:36.2_0": "ekampi jātiṁ dvepi jātiyo …pe…",
  "ps2.9:36.3_0": "iti sākāraṁ sauddesaṁ anekavihitaṁ pubbenivāsaṁ anussarati.",
  "ps2.9:36.4_0": "Yampi tathāgato anekavihitaṁ pubbenivāsaṁ anussarati.",
  "ps2.9:36.5_0": "Seyyathidaṁ—",
  "ps2.9:36.6_0": "ekampi jātiṁ dvepi jātiyo …pe…",
  "ps2.9:36.7_0": "idampi tathāgatassa …pe…. ",
  "ps2.9:37.1_0": "Puna caparaṁ tathāgato dibbena cakkhunā visuddhena atikkantamānusakena satte passati cavamāne upapajjamāne …pe…",
  "ps2.9:37.2_0": "yampi tathāgato dibbena cakkhunā visuddhena atikkantamānusakena satte passati cavamāne upapajjamāne …pe…",
  "ps2.9:37.3_0": "idampi tathāgatassa …pe…. ",
  "ps2.9:38.1_0": "Puna caparaṁ tathāgato āsavānaṁ khayā anāsavaṁ cetovimuttiṁ paññāvimuttiṁ diṭṭheva dhamme sayaṁ abhiññā sacchikatvā upasampajja viharati.",
  "ps2.9:38.2_0": "Yampi tathāgato āsavānaṁ khayā anāsavaṁ cetovimuttiṁ paññāvimuttiṁ diṭṭheva dhamme sayaṁ abhiññā sacchikatvā upasampajja viharati,",
  "ps2.9:38.2_1": "idampi tathāgatassa tathāgatabalaṁ hoti,",
  "ps2.9:38.2_2": "yaṁ balaṁ āgamma tathāgato āsabhaṁ ṭhānaṁ paṭijānāti,",
  "ps2.9:38.2_3": "parisāsu sīhanādaṁ nadati,",
  "ps2.9:38.2_4": "brahmacakkaṁ pavatteti.",
  "ps2.9:38.3_0": "Imāni dasa tathāgatabalāni. ",
  "ps2.9:39.1_0": "Kenaṭṭhena saddhābalaṁ?",
  "ps2.9:39.2_0": "Kenaṭṭhena vīriyabalaṁ?",
  "ps2.9:39.3_0": "Kenaṭṭhena satibalaṁ?",
  "ps2.9:39.4_0": "Kenaṭṭhena samādhibalaṁ?",
  "ps2.9:39.5_0": "Kenaṭṭhena paññābalaṁ?",
  "ps2.9:39.6_0": "Kenaṭṭhena hiribalaṁ?",
  "ps2.9:39.7_0": "Kenaṭṭhena ottappabalaṁ?",
  "ps2.9:39.8_0": "Kenaṭṭhena paṭisaṅkhānabalaṁ …pe…",
  "ps2.9:39.9_0": "kenaṭṭhena tathāgatabalaṁ?",
  "ps2.9:40.1_0": "Assaddhiye akampiyaṭṭhena saddhābalaṁ.",
  "ps2.9:40.2_0": "Kosajje akampiyaṭṭhena vīriyabalaṁ.",
  "ps2.9:40.3_0": "Pamāde akampiyaṭṭhena satibalaṁ.",
  "ps2.9:40.4_0": "Uddhacce akampiyaṭṭhena samādhibalaṁ.",
  "ps2.9:40.5_0": "Avijjāya akampiyaṭṭhena paññābalaṁ.",
  "ps2.9:40.6_0": "Hirīyati pāpake akusale dhammeti—hiribalaṁ.",
  "ps2.9:40.7_0": "Ottappati pāpake akusale dhammeti—ottappabalaṁ.",
  "ps2.9:40.8_0": "Ñāṇena kilese paṭisaṅkhātīti—paṭisaṅkhānabalaṁ.",
  "ps2.9:40.9_0": "Tattha jātā dhammā ekarasā hontīti— bhāvanābalaṁ.",
  "ps2.9:40.10_0": "Tattha natthi kiñci vajjanti—anavajjabalaṁ.",
  "ps2.9:40.11_0": "Tena cittaṁ saṅgaṇhātīti—saṅgahabalaṁ.",
  "ps2.9:40.12_0": "Taṁ tassa khamatīti—",
  "ps2.9:40.13_0": "khantibalaṁ.",
  "ps2.9:40.14_0": "Tena cittaṁ paññapetīti—paññattibalaṁ.",
  "ps2.9:40.15_0": "Tena cittaṁ nijjhāpetīti—nijjhattibalaṁ.",
  "ps2.9:40.16_0": "Tena cittaṁ vasaṁ vattetīti—issariyabalaṁ.",
  "ps2.9:40.17_0": "Tena cittaṁ adhiṭṭhātīti— adhiṭṭhānabalaṁ.",
  "ps2.9:40.18_0": "Tena cittaṁ ekagganti—samathabalaṁ.",
  "ps2.9:40.19_0": "Tattha jāte dhamme anupassatīti—vipassanābalaṁ.",
  "ps2.9:40.20_0": "Tattha sikkhatīti—sekhabalaṁ.",
  "ps2.9:40.21_0": "Tattha sikkhitattā—",
  "ps2.9:40.22_0": "asekhabalaṁ.",
  "ps2.9:40.23_0": "Tena āsavā khīṇāti—khīṇāsavabalaṁ.",
  "ps2.9:40.24_0": "Tassa ijjhatīti—iddhibalaṁ.",
  "ps2.9:40.25_0": "Appameyyaṭṭhena tathāgatabalanti.",
  "ps2.9:40.26_0": "Balakathā niṭṭhitā."
}