{
  "thi-ap7:0.1_0": "Therīapadāna",
  "thi-ap7:0.2_0": "Sumedhāvagga",
  "thi-ap7:0.3_0": "7 Kaṭacchubhikkhādāyikātherīapadāna",
  "thi-ap7:1.1_0": "“Piṇḍacāraṁ carantassa,",
  "thi-ap7:1.2_0": "tissanāmassa satthuno;",
  "thi-ap7:1.3_0": "Kaṭacchubhikkhaṁ paggayha,",
  "thi-ap7:1.4_0": "buddhaseṭṭhassa dāsahaṁ.",
  "thi-ap7:2.1_0": "Paṭiggahetvā sambuddho,",
  "thi-ap7:2.2_0": "tisso lokagganāyako;",
  "thi-ap7:2.3_0": "Vīthiyā saṇṭhito satthā,",
  "thi-ap7:2.4_0": "akā me anumodanaṁ.",
  "thi-ap7:3.1_0": "‘Kaṭacchubhikkhaṁ datvāna,",
  "thi-ap7:3.2_0": "tāvatiṁsaṁ gamissasi;",
  "thi-ap7:3.3_0": "Chattiṁsadevarājūnaṁ,",
  "thi-ap7:3.4_0": "mahesittaṁ karissasi.",
  "thi-ap7:4.1_0": "Paññāsaṁ cakkavattīnaṁ,",
  "thi-ap7:4.2_0": "mahesittaṁ karissasi;",
  "thi-ap7:4.3_0": "Manasā patthitaṁ sabbaṁ,",
  "thi-ap7:4.4_0": "paṭilacchasi sabbadā.",
  "thi-ap7:5.1_0": "Sampattiṁ anubhotvāna,",
  "thi-ap7:5.2_0": "pabbajissasikiñcanā;",
  "thi-ap7:5.3_0": "Sabbāsave pariññāya,",
  "thi-ap7:5.4_0": "nibbāyissasināsavā’.",
  "thi-ap7:6.1_0": "Idaṁ vatvāna sambuddho,",
  "thi-ap7:6.2_0": "tisso lokagganāyako;",
  "thi-ap7:6.3_0": "Nabhaṁ abbhuggamī vīro,",
  "thi-ap7:6.4_0": "haṁsarājāva ambare.",
  "thi-ap7:7.1_0": "Sudinnaṁ me dānavaraṁ,",
  "thi-ap7:7.2_0": "suyiṭṭhā yāgasampadā;",
  "thi-ap7:7.3_0": "Kaṭacchubhikkhaṁ datvāna,",
  "thi-ap7:7.4_0": "pattāhaṁ acalaṁ padaṁ.",
  "thi-ap7:8.1_0": "Dvenavute ito kappe,",
  "thi-ap7:8.2_0": "yaṁ dānamadadiṁ tadā;",
  "thi-ap7:8.3_0": "Duggatiṁ nābhijānāmi,",
  "thi-ap7:8.4_0": "bhikkhādānassidaṁ phalaṁ.",
  "thi-ap7:9.1_0": "Kilesā jhāpitā mayhaṁ,",
  "thi-ap7:9.2_0": "…pe…",
  "thi-ap7:9.3_0": "viharāmi anāsavā.",
  "thi-ap7:10.1_0": "Svāgataṁ vata me āsi,",
  "thi-ap7:10.2_0": "…pe…",
  "thi-ap7:10.3_0": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap7:11.1_0": "Paṭisambhidā catasso,",
  "thi-ap7:11.2_0": "…pe…",
  "thi-ap7:11.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "thi-ap7:12.1_0": "Itthaṁ sudaṁ kaṭacchubhikkhādāyikā bhikkhunī imā gāthāyo abhāsitthāti.",
  "thi-ap7:13.1_0": "Kaṭacchubhikkhādāyikātheriyāpadānaṁ sattamaṁ."
}