{
  "tha-ap130:0.1_0": "Therāpadāna",
  "tha-ap130:0.2_0": "Sereyyavagga",
  "tha-ap130:0.3_0": "8 Ñāṇasaññikattheraapadāna",
  "tha-ap130:1.1_0": "“Pabbate himavantamhi,",
  "tha-ap130:1.2_0": "vasāmi pabbatantare;",
  "tha-ap130:1.3_0": "Pulinaṁ sobhanaṁ disvā,",
  "tha-ap130:1.4_0": "buddhaseṭṭhaṁ anussariṁ.",
  "tha-ap130:2.1_0": "Ñāṇe upanidhā natthi,",
  "tha-ap130:2.2_0": "saṅkhāraṁ natthi satthuno;",
  "tha-ap130:2.3_0": "Sabbadhammaṁ abhiññāya,",
  "tha-ap130:2.4_0": "ñāṇena adhimuccati.",
  "tha-ap130:3.1_0": "Namo te purisājañña,",
  "tha-ap130:3.2_0": "namo te purisuttama;",
  "tha-ap130:3.3_0": "Ñāṇena te samo natthi,",
  "tha-ap130:3.4_0": "yāvatā ñāṇamuttamaṁ.",
  "tha-ap130:4.1_0": "Ñāṇe cittaṁ pasādetvā,",
  "tha-ap130:4.2_0": "kappaṁ saggamhi modahaṁ;",
  "tha-ap130:4.3_0": "Avasesesu kappesu,",
  "tha-ap130:4.4_0": "kusalaṁ caritaṁ mayā.",
  "tha-ap130:5.1_0": "Ekanavutito kappe,",
  "tha-ap130:5.2_0": "yaṁ saññamalabhiṁ tadā;",
  "tha-ap130:5.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap130:5.4_0": "ñāṇasaññāyidaṁ phalaṁ.",
  "tha-ap130:6.1_0": "Ito sattatikappamhi,",
  "tha-ap130:6.2_0": "eko pulinapupphiyo;",
  "tha-ap130:6.3_0": "Sattaratanasampanno,",
  "tha-ap130:6.4_0": "cakkavattī mahabbalo.",
  "tha-ap130:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap130:7.2_0": "…pe…",
  "tha-ap130:7.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap130:8.1_0": "Itthaṁ sudaṁ āyasmā ñāṇasaññiko thero imā gāthāyo abhāsitthāti.",
  "tha-ap130:9.1_0": "Ñāṇasaññikattherassāpadānaṁ aṭṭhamaṁ."
}