{
  "ja320:0.1_0": "Jātaka",
  "ja320:0.2_0": "Catukkanipāta",
  "ja320:0.3_0": "Pucimandavagga",
  "ja320:0.4_0": "10. Succajajātaka",
  "ja320:1.1_0": "“Succajaṁ vata naccaji,",
  "ja320:1.2_0": "vācāya adadaṁ giriṁ;",
  "ja320:1.3_0": "Kiṁ hitassa cajantassa,",
  "ja320:1.4_0": "vācāya adada pabbataṁ”.",
  "ja320:2.1_0": "“Yañhi kayirā tañhi vade,",
  "ja320:2.2_0": "yaṁ na kayirā na taṁ vade;",
  "ja320:2.3_0": "Akarontaṁ bhāsamānaṁ,",
  "ja320:2.4_0": "parijānanti paṇḍitā”.",
  "ja320:3.1_0": "“Rājaputta namo tyatthu,",
  "ja320:3.2_0": "sacce dhamme ṭhito casi;",
  "ja320:3.3_0": "Yassa te byasanaṁ patto,",
  "ja320:3.4_0": "saccasmiṁ ramate mano”.",
  "ja320:4.1_0": "“Yā daliddī daliddassa,",
  "ja320:4.2_0": "aḍḍhā aḍḍhassa kittima;",
  "ja320:4.3_0": "Sā hissa paramā bhariyā,",
  "ja320:4.4_0": "sahiraññassa itthiyo”ti.",
  "ja320:5.1_0": "Succajajātakaṁ dasamaṁ.",
  "ja320:5.2_0": "Pucimandavaggo dutiyo.",
  "ja320:6.1_0": "Tassuddānaṁ",
  "ja320:7.1_0": "Atha cora sakassapa khantīvaro,",
  "ja320:7.2_0": "Dujjīvitatā ca varā pharusā;",
  "ja320:7.3_0": "Atha sasa matañca vasanta sukhaṁ,",
  "ja320:7.4_0": "Succajaṁvatanaccajinā ca dasāti."
}