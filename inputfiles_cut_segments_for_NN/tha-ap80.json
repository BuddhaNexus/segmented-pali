{
  "tha-ap80:0.1_0": "Therāpadāna",
  "tha-ap80:0.2_0": "Nāgasamālavagga",
  "tha-ap80:0.3_0": "8. Pāṭalipupphiyattheraapadāna",
  "tha-ap80:1.1_0": "“Suvaṇṇavaṇṇaṁ sambuddhaṁ,",
  "tha-ap80:1.2_0": "Gacchantaṁ antarāpaṇe;",
  "tha-ap80:1.3_0": "Kañcanagghiyasaṅkāsaṁ,",
  "tha-ap80:1.4_0": "Bāttiṁsavaralakkhaṇaṁ.",
  "tha-ap80:2.1_0": "Seṭṭhiputto tadā āsiṁ,",
  "tha-ap80:2.2_0": "sukhumālo sukhedhito;",
  "tha-ap80:2.3_0": "Ucchaṅge pāṭalipupphaṁ,",
  "tha-ap80:2.4_0": "katvāna abhisaṁhariṁ.",
  "tha-ap80:3.1_0": "Haṭṭho haṭṭhena cittena,",
  "tha-ap80:3.2_0": "pupphehi abhipūjayiṁ;",
  "tha-ap80:3.3_0": "Tissaṁ lokaviduṁ nāthaṁ,",
  "tha-ap80:3.4_0": "naradevaṁ namassahaṁ.",
  "tha-ap80:4.1_0": "Dvenavute ito kappe,",
  "tha-ap80:4.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap80:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap80:4.4_0": "pupphapūjāyidaṁ phalaṁ.",
  "tha-ap80:5.1_0": "Ito tesaṭṭhikappamhi,",
  "tha-ap80:5.2_0": "abhisammatanāmako;",
  "tha-ap80:5.3_0": "Sattaratanasampanno,",
  "tha-ap80:5.4_0": "cakkavattī mahabbalo.",
  "tha-ap80:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap80:6.2_0": "…pe…",
  "tha-ap80:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap80:7.1_0": "Itthaṁ sudaṁ āyasmā pāṭalipupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap80:8.1_0": "Pāṭalipupphiyattherassāpadānaṁ aṭṭhamaṁ."
}