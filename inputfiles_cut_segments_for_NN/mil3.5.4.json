{
  "mil3.5.4:0.1_0": "Milindapañha",
  "mil3.5.4:0.2_0": "Buddhavagga",
  "mil3.5.4:0.3_0": "4. Dhammadiṭṭhapañha",
  "mil3.5.4:1.1_0": "Rājā āha—",
  "mil3.5.4:1.2_0": "“bhante nāgasena, dhammo tayā diṭṭho”ti.",
  "mil3.5.4:1.3_0": "“Buddhanettiyā kho, mahārāja,",
  "mil3.5.4:1.3_1": "buddhapaññattiyā yāvajīvaṁ sāvakehi vattitabban”ti.",
  "mil3.5.4:2.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.5.4:3.1_0": "Dhammadiṭṭhapañho catuttho."
}