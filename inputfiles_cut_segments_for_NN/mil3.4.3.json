{
  "mil3.4.3:0.1_0": "Milindapañha",
  "mil3.4.3:0.2_0": "Nibbānavagga",
  "mil3.4.3:0.3_0": "3. Pañcāyatanakammanibbattapañha",
  "mil3.4.3:1.1_0": "Rājā āha—",
  "mil3.4.3:1.2_0": "“bhante nāgasena,",
  "mil3.4.3:1.2_1": "yānimāni pañcāyatanāni,",
  "mil3.4.3:1.2_2": "kiṁ nu tāni nānākammehi nibbattāni,",
  "mil3.4.3:1.2_3": "udāhu ekena kammenā”ti?",
  "mil3.4.3:1.3_0": "“Nānākammehi, mahārāja, nibbattāni, na ekena kammenā”ti.",
  "mil3.4.3:2.1_0": "“Opammaṁ karohī”ti.",
  "mil3.4.3:2.2_0": "“Taṁ kiṁ maññasi, mahārāja,",
  "mil3.4.3:2.2_1": "ekasmiṁ khette nānābījāni vappeyyuṁ,",
  "mil3.4.3:2.2_2": "tesaṁ nānābījānaṁ nānāphalāni nibbatteyyun”ti?",
  "mil3.4.3:2.3_0": "“Āma, bhante, nibbatteyyun”ti.",
  "mil3.4.3:2.4_0": "“Evameva kho, mahārāja,",
  "mil3.4.3:2.4_1": "yāni yāni pañcāyatanāni,",
  "mil3.4.3:2.4_2": "tāni tāni nānākammehi nibbattāni,",
  "mil3.4.3:2.4_3": "na ekena kammenā”ti.",
  "mil3.4.3:3.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.4.3:4.1_0": "Pañcāyatanakammanibbattapañho tatiyo."
}