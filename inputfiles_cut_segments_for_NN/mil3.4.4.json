{
  "mil3.4.4:0.1_0": "Milindapañha",
  "mil3.4.4:0.2_0": "Nibbānavagga",
  "mil3.4.4:0.3_0": "4. Kammanānākaraṇapañha",
  "mil3.4.4:1.1_0": "Rājā āha—",
  "mil3.4.4:1.2_0": "“bhante nāgasena,",
  "mil3.4.4:1.2_1": "kena kāraṇena manussā na sabbe samakā,",
  "mil3.4.4:1.2_2": "aññe appāyukā,",
  "mil3.4.4:1.2_3": "aññe dīghāyukā,",
  "mil3.4.4:1.2_4": "aññe bahvābādhā aññe appābādhā,",
  "mil3.4.4:1.2_5": "aññe dubbaṇṇā,",
  "mil3.4.4:1.2_6": "aññe vaṇṇavanto,",
  "mil3.4.4:1.2_7": "aññe appesakkhā,",
  "mil3.4.4:1.2_8": "aññe mahesakkhā,",
  "mil3.4.4:1.2_9": "aññe appabhogā,",
  "mil3.4.4:1.2_10": "aññe mahābhogā,",
  "mil3.4.4:1.2_11": "aññe nīcakulīnā,",
  "mil3.4.4:1.2_12": "aññe mahākulīnā,",
  "mil3.4.4:1.2_13": "aññe duppaññā,",
  "mil3.4.4:1.2_14": "aññe paññavanto”ti?",
  "mil3.4.4:2.1_0": "Thero āha—",
  "mil3.4.4:2.2_0": "“kissa pana, mahārāja,",
  "mil3.4.4:2.2_1": "rukkhā na sabbe samakā,",
  "mil3.4.4:2.2_2": "aññe ambilā,",
  "mil3.4.4:2.2_3": "aññe lavaṇā,",
  "mil3.4.4:2.2_4": "aññe tittakā,",
  "mil3.4.4:2.2_5": "aññe kaṭukā,",
  "mil3.4.4:2.2_6": "aññe kasāvā,",
  "mil3.4.4:2.2_7": "aññe madhurā”ti?",
  "mil3.4.4:2.3_0": "“Maññāmi, bhante, bījānaṁ nānākaraṇenā”ti.",
  "mil3.4.4:2.4_0": "“Evameva kho, mahārāja,",
  "mil3.4.4:2.4_1": "kammānaṁ nānākaraṇena manussā na sabbe samakā,",
  "mil3.4.4:2.4_2": "aññe appāyukā,",
  "mil3.4.4:2.4_3": "aññe dīghāyukā,",
  "mil3.4.4:2.4_4": "aññe bahvābādhā,",
  "mil3.4.4:2.4_5": "aññe appābādhā,",
  "mil3.4.4:2.4_6": "aññe dubbaṇṇā,",
  "mil3.4.4:2.4_7": "aññe vaṇṇavanto,",
  "mil3.4.4:2.4_8": "aññe appesakkhā,",
  "mil3.4.4:2.4_9": "aññe mahesakkhā,",
  "mil3.4.4:2.4_10": "aññe appabhogā,",
  "mil3.4.4:2.4_11": "aññe mahābhogā,",
  "mil3.4.4:2.4_12": "aññe nīcakulīnā,",
  "mil3.4.4:2.4_13": "aññe mahākulīnā,",
  "mil3.4.4:2.4_14": "aññe duppaññā,",
  "mil3.4.4:2.4_15": "aññe paññavanto.",
  "mil3.4.4:2.5_0": "Bhāsitampetaṁ, mahārāja, bhagavatā—",
  "mil3.4.4:2.6_0": "‘kammassakā, māṇava,",
  "mil3.4.4:2.6_1": "sattā kammadāyādā kammayonī kammabandhū kammappaṭisaraṇā,",
  "mil3.4.4:2.6_2": "kammaṁ satte vibhajati yadidaṁ hīnappaṇītatāyā’”ti.",
  "mil3.4.4:3.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.4.4:4.1_0": "Kammanānākaraṇapañho catuttho."
}