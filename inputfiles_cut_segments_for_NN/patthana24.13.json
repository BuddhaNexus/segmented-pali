{
  "patthana24.13:0.1_0": "Paṭṭhānapakaraṇa",
  "patthana24.13:0.2_0": "Dhammapaccanīyānuloma (4), Dukadukapaṭṭhānapāḷi (6)",
  "patthana24.13:0.3_0": "24.13. Mahantaradukādi, Hetuduka",
  "patthana24.13:1.1_0": "Nasārammaṇaṁ nahetuṁ dhammaṁ paṭicca sārammaṇo hetu dhammo uppajjati hetupaccayā.",
  "patthana24.13:1.1_1": "(Saṅkhittaṁ.) Hetuyā tīṇi,",
  "patthana24.13:1.1_2": "ārammaṇe tīṇi,",
  "patthana24.13:1.1_3": "adhipatiyā ekaṁ …pe…",
  "patthana24.13:1.1_4": "purejāte āsevane ekaṁ …pe…",
  "patthana24.13:1.1_5": "avigate tīṇi.",
  "patthana24.13:2.1_0": "Naanārammaṇaṁ nanahetuṁ dhammaṁ paṭicca sārammaṇo nahetu dhammo uppajjati hetupaccayā.",
  "patthana24.13:2.1_1": "(Saṅkhittaṁ.) Hetuyā tīṇi,",
  "patthana24.13:2.1_2": "ārammaṇe ekaṁ …pe…",
  "patthana24.13:2.1_3": "avigate tīṇi."
}