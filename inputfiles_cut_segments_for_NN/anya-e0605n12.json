{
  "anya-e0605n12:0_0": "Dvipādabyāsayamakagāthā",
  "anya-e0605n12:1_0": "87.",
  "anya-e0605n12:2_0": " Yasodharaṁ pīṇapayodharādharaṁ,",
  "anya-e0605n12:3_0": "Anaṅgaraṅgaddhajabhūtamaṅgaṁ;",
  "anya-e0605n12:4_0": "Devaccharāvujjalitaṁ patibbataṁ,",
  "anya-e0605n12:5_0": "Hitvā gato so sugato va nūna.",
  "anya-e0605n12:6_0": "88.",
  "anya-e0605n12:7_0": " Sabhāvanicchandamatiṁ pabhāvatiṁ,",
  "anya-e0605n12:8_0": "Bhatto kuso saṁhari bhattakājaṁ;",
  "anya-e0605n12:9_0": "Tāyābhirūpaṁ pi yasodharaṁ varaṁ,",
  "anya-e0605n12:10_0": "Hitvā gato so sugato va nūna.",
  "anya-e0605n12:11_0": "89.",
  "anya-e0605n12:12_0": " Pure pure saṇcari khaggahattho,",
  "anya-e0605n12:13_0": "Varaṁ paritthīnaṁ anitthigandho;",
  "anya-e0605n12:14_0": "Siriñca riñcāpi na riñci nāriṁ,",
  "anya-e0605n12:15_0": "Hitvānimandāni gato tathāgato.",
  "anya-e0605n12:16_0": "90.",
  "anya-e0605n12:17_0": " Harittaco rāgabalena deviyā,",
  "anya-e0605n12:18_0": "Avatthaliṅgena na liṅganussari;",
  "anya-e0605n12:19_0": "Asevi kāmaṁ tamidāni kāmaṁ,",
  "anya-e0605n12:20_0": "Hitvā gato so sugato va nūna.",
  "anya-e0605n12:21_0": "91.",
  "anya-e0605n12:22_0": " Apameyyakappesu vivekasevī,",
  "anya-e0605n12:23_0": "Hitvā gato rajjasiriṁ varitthiṁ;",
  "anya-e0605n12:24_0": "Aṇuṁ kaliṁ vaṇṇayi taṁ purāṇaṁ,",
  "anya-e0605n12:25_0": "Vatthamhi chiddaṁ viya tunnakāro.",
  "anya-e0605n12:26_0": "92.",
  "anya-e0605n12:27_0": " Tathāti mantvāna idāninaṅgo,",
  "anya-e0605n12:28_0": "Yasodharaṁ paggahito dhajaṁ va;",
  "anya-e0605n12:29_0": "Matto jitomhī ti pamattabandhu,",
  "anya-e0605n12:30_0": "Na passi ñāṇāsanipātamantaraṁ.",
  "anya-e0605n12:31_0": "93.",
  "anya-e0605n12:32_0": " Disvāna dukkhānalasambhavaṁbhavaṁ,",
  "anya-e0605n12:33_0": "Katvā taduppādakanaṅgabhaṅgaṁ;",
  "anya-e0605n12:34_0": "Yasodharaṁ pīṇapayodharādharaṁ,",
  "anya-e0605n12:35_0": "Hitvā gato buddhabalappadaṁ padaṁ.",
  "anya-e0605n12:36_0": "94.",
  "anya-e0605n12:37_0": " Anantasattānamanantakāle,",
  "anya-e0605n12:38_0": "Manaṅgahetvāna jito anaṅgo;",
  "anya-e0605n12:39_0": "Parājito nūna hi ekakassa,",
  "anya-e0605n12:40_0": "Tathāgato so na punāgato va.",
  "anya-e0605n12:41_0": "95.",
  "anya-e0605n12:42_0": " Disvāna ñāṇāsanipātamantaraṁ,",
  "anya-e0605n12:43_0": "Tathāgato so na punāgato va;",
  "anya-e0605n12:44_0": "Tathāgato so na punāgato va,",
  "anya-e0605n12:45_0": "Disvānañāṇāsanipātamantaraṁ."
}