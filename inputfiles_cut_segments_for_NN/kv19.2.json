{
  "kv19.2:0.1_0": "Kathāvatthu",
  "kv19.2:0.2_0": "Mahāpaṇṇāsaka",
  "kv19.2:0.3_0": "Ekūnavīsatimavagga",
  "kv19.2:0.4_0": "Suññatākathā",
  "kv19.2:1.1_0": "Suññatā saṅkhārakkhandhapariyāpannāti?",
  "kv19.2:1.2_0": "Āmantā.",
  "kv19.2:1.3_0": "Animittaṁ saṅkhārakkhandhapariyāpannanti?",
  "kv19.2:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv19.2:1.5_0": "suññatā saṅkhārakkhandhapariyāpannāti?",
  "kv19.2:1.6_0": "Āmantā.",
  "kv19.2:1.7_0": "Appaṇihito saṅkhārakkhandhapariyāpannoti?",
  "kv19.2:1.8_0": "Na hevaṁ vattabbe …pe…",
  "kv19.2:1.9_0": "animittaṁ na vattabbaṁ—",
  "kv19.2:1.10_0": "“saṅkhārakkhandhapariyāpannan”ti?",
  "kv19.2:1.11_0": "Āmantā.",
  "kv19.2:1.12_0": "Suññatā na vattabbā—",
  "kv19.2:1.13_0": "“saṅkhārakkhandhapariyāpannā”ti?",
  "kv19.2:1.14_0": "Na hevaṁ vattabbe …pe…",
  "kv19.2:1.15_0": "appaṇihito na vattabbo—",
  "kv19.2:1.16_0": "“saṅkhārakkhandhapariyāpanno”ti?",
  "kv19.2:1.17_0": "Āmantā.",
  "kv19.2:1.18_0": "Suññatā na vattabbā—",
  "kv19.2:1.19_0": "“saṅkhārakkhandhapariyāpannā”ti?",
  "kv19.2:1.20_0": "Na hevaṁ vattabbe …pe….",
  "kv19.2:2.1_0": "Suññatā saṅkhārakkhandhapariyāpannāti?",
  "kv19.2:2.2_0": "Āmantā.",
  "kv19.2:2.3_0": "Saṅkhārakkhandho na anicco na saṅkhato na paṭiccasamuppanno na khayadhammo na vayadhammo na virāgadhammo na nirodhadhammo na vipariṇāmadhammoti?",
  "kv19.2:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv19.2:2.5_0": "nanu saṅkhārakkhandho anicco saṅkhato paṭiccasamuppanno khayadhammo vayadhammo virāgadhammo nirodhadhammo vipariṇāmadhammoti?",
  "kv19.2:2.6_0": "Āmantā.",
  "kv19.2:2.7_0": "Hañci saṅkhārakkhandho anicco …pe…",
  "kv19.2:2.8_0": "vipariṇāmadhammo, no ca vata re vattabbe—",
  "kv19.2:2.9_0": "“suññatā saṅkhārakkhandhapariyāpannā”ti.",
  "kv19.2:3.1_0": "Rūpakkhandhassa suññatā saṅkhārakkhandhapariyāpannāti?",
  "kv19.2:3.2_0": "Āmantā.",
  "kv19.2:3.3_0": "Saṅkhārakkhandhassa suññatā rūpakkhandhapariyāpannāti?",
  "kv19.2:3.4_0": "Na hevaṁ vattabbe …pe…",
  "kv19.2:3.5_0": "vedanākkhandhassa …pe…",
  "kv19.2:3.6_0": "saññākkhandhassa …",
  "kv19.2:3.7_0": "viññāṇakkhandhassa suññatā saṅkhārakkhandhapariyāpannāti?",
  "kv19.2:3.8_0": "Āmantā.",
  "kv19.2:3.9_0": "Saṅkhārakkhandhassa suññatā viññāṇakkhandhapariyāpannāti?",
  "kv19.2:3.10_0": "Na hevaṁ vattabbe …pe….",
  "kv19.2:4.1_0": "Saṅkhārakkhandhassa suññatā na vattabbā—",
  "kv19.2:4.2_0": "“rūpakkhandhapariyāpannā”ti?",
  "kv19.2:4.3_0": "Āmantā.",
  "kv19.2:4.4_0": "Rūpakkhandhassa suññatā na vattabbā—",
  "kv19.2:4.5_0": "“saṅkhārakkhandhapariyāpannā”ti?",
  "kv19.2:4.6_0": "Na hevaṁ vattabbe …pe…",
  "kv19.2:4.7_0": "saṅkhārakkhandhassa suññatā na vattabbā—",
  "kv19.2:4.8_0": "“vedanākkhandhapariyāpannā …pe…",
  "kv19.2:4.9_0": "saññākkhandhapariyāpannā …",
  "kv19.2:4.10_0": "viññāṇakkhandhapariyāpannā”ti?",
  "kv19.2:4.11_0": "Āmantā.",
  "kv19.2:4.12_0": "Viññāṇakkhandhassa suññatā na vattabbā—",
  "kv19.2:4.13_0": "“saṅkhārakkhandhapariyāpannā”ti?",
  "kv19.2:4.14_0": "Na hevaṁ vattabbe …pe….",
  "kv19.2:5.1_0": "Na vattabbaṁ—",
  "kv19.2:5.2_0": "“suññatā saṅkhārakkhandhapariyāpannā”ti?",
  "kv19.2:5.3_0": "Āmantā.",
  "kv19.2:5.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv19.2:5.5_0": "“suññamidaṁ, bhikkhave,",
  "kv19.2:5.5_1": "saṅkhārā attena vā attaniyena vā”ti.",
  "kv19.2:5.6_0": "Attheva suttantoti?",
  "kv19.2:5.7_0": "Āmantā.",
  "kv19.2:5.8_0": "Tena hi suññatā saṅkhārakkhandhapariyāpannāti.",
  "kv19.2:5.9_0": "Suññatākathā niṭṭhitā."
}