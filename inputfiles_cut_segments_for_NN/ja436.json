{
  "ja436:0.1_0": "Jātaka",
  "ja436:0.2_0": "Navakanipāta",
  "ja436:0.3_0": "Gijjhavagga",
  "ja436:0.4_0": "10. Samuggajātaka",
  "ja436:1.1_0": "“Kuto nu āgacchatha bho tayo janā,",
  "ja436:1.2_0": "Svāgatā etha nisīdathāsane;",
  "ja436:1.3_0": "Kaccittha bhonto kusalaṁ anāmayaṁ,",
  "ja436:1.4_0": "Cirassamabbhāgamanaṁ hi vo idha”.",
  "ja436:2.1_0": "“Ahameva eko idha majja patto,",
  "ja436:2.2_0": "Na cāpi me dutiyo koci vijjati;",
  "ja436:2.3_0": "Kimeva sandhāya te bhāsitaṁ ise,",
  "ja436:2.4_0": "‘Kuto nu āgacchatha bho tayo janā’”.",
  "ja436:3.1_0": "“Tuvañca eko bhariyā ca te piyā,",
  "ja436:3.2_0": "Samuggapakkhittanikiṇṇamantare;",
  "ja436:3.3_0": "Sā rakkhitā kucchigatāva te sadā,",
  "ja436:3.4_0": "Vāyussa puttena sahā tahiṁ ratā”.",
  "ja436:4.1_0": "“Saṁviggarūpo isinā viyākato,",
  "ja436:4.2_0": "So dānavo tattha samuggamuggili;",
  "ja436:4.3_0": "Addakkhi bhariyaṁ suci māladhāriniṁ,",
  "ja436:4.4_0": "Vāyussa puttena sahā tahiṁ rataṁ”.",
  "ja436:5.1_0": "“Sudiṭṭharūpamuggatapānuvattinā,",
  "ja436:5.2_0": "Hīnā narā ye pamadāvasaṁ gatā;",
  "ja436:5.3_0": "Yathā have pāṇarivettha rakkhitā,",
  "ja436:5.4_0": "Duṭṭhā mayī aññamabhippamodayi.",
  "ja436:6.1_0": "Divā ca ratto ca mayā upaṭṭhitā,",
  "ja436:6.2_0": "Tapassinā jotirivā vane vasaṁ;",
  "ja436:6.3_0": "Sā dhammamukkamma adhammamācari,",
  "ja436:6.4_0": "Akiriyarūpo pamadāhi santhavo.",
  "ja436:7.1_0": "Sarīramajjhamhi ṭhitātimaññahaṁ,",
  "ja436:7.2_0": "Mayhaṁ ayanti asatiṁ asaññataṁ;",
  "ja436:7.3_0": "Sā dhammamukkamma adhammamācari,",
  "ja436:7.4_0": "Akiriyarūpo pamadāhi santhavo.",
  "ja436:8.1_0": "Surakkhitaṁ meti kathaṁ nu vissase,",
  "ja436:8.2_0": "Anekacittāsu na hatthi rakkhaṇā;",
  "ja436:8.3_0": "Etā hi pātālapapātasannibhā,",
  "ja436:8.4_0": "Etthappamatto byasanaṁ nigaccha”ti.",
  "ja436:9.1_0": "“Tasmā hi te sukhino vītasokā,",
  "ja436:9.2_0": "Ye mātugāmehi caranti nissaṭā;",
  "ja436:9.3_0": "Etaṁ sivaṁ uttamamābhipatthayaṁ,",
  "ja436:9.4_0": "Na mātugāmehi kareyya santhavanti”.",
  "ja436:10.1_0": "Samuggajātakaṁ dasamaṁ."
}