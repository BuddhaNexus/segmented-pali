{
  "ja415:0.1_0": "Jātaka",
  "ja415:0.2_0": "Sattakanipāta",
  "ja415:0.3_0": "Gandhāravagga",
  "ja415:0.4_0": "10. Kummāsapiṇḍijātaka",
  "ja415:1.1_0": "“Na kiratthi anomadassisu,",
  "ja415:1.2_0": "Pāricariyā buddhesu appikā;",
  "ja415:1.3_0": "Sukkhāya aloṇikāya ca,",
  "ja415:1.4_0": "Passaphalaṁ kummāsapiṇḍiyā.",
  "ja415:2.1_0": "Hatthigavassā cime bahū,",
  "ja415:2.2_0": "Dhanadhaññaṁ pathavī ca kevalā;",
  "ja415:2.3_0": "Nāriyo cimā accharūpamā,",
  "ja415:2.4_0": "Passa phalaṁ kummāsapiṇḍiyā”.",
  "ja415:3.1_0": "“Abhikkhaṇaṁ rājakuñjara,",
  "ja415:3.2_0": "Gāthā bhāsasi kosalādhipa;",
  "ja415:3.3_0": "Pucchāmi taṁ raṭṭhavaḍḍhana,",
  "ja415:3.4_0": "Bāḷhaṁ pītimano pabhāsasi”.",
  "ja415:4.1_0": "“Imasmiññeva nagare,",
  "ja415:4.2_0": "kule aññatare ahuṁ;",
  "ja415:4.3_0": "Parakammakaro āsiṁ,",
  "ja415:4.4_0": "bhatako sīlasaṁvuto.",
  "ja415:5.1_0": "Kammāya nikkhamantohaṁ,",
  "ja415:5.2_0": "caturo samaṇeddasaṁ;",
  "ja415:5.3_0": "Ācārasīlasampanne,",
  "ja415:5.4_0": "sītibhūte anāsave.",
  "ja415:6.1_0": "Tesu cittaṁ pasādetvā,",
  "ja415:6.2_0": "nisīdetvā paṇṇasanthate;",
  "ja415:6.3_0": "Adaṁ buddhāna kummāsaṁ,",
  "ja415:6.4_0": "pasanno sehi pāṇibhi.",
  "ja415:7.1_0": "Tassa kammassa kusalassa,",
  "ja415:7.2_0": "idaṁ me edisaṁ phalaṁ;",
  "ja415:7.3_0": "Anubhomi idaṁ rajjaṁ,",
  "ja415:7.4_0": "phītaṁ dharaṇimuttamaṁ”.",
  "ja415:8.1_0": "“Dadaṁ bhuñja mā ca pamādo,",
  "ja415:8.2_0": "Cakkaṁ vattaya kosalādhipa;",
  "ja415:8.3_0": "Mā rāja adhammiko ahu,",
  "ja415:8.4_0": "Dhammaṁ pālaya kosalādhipa”.",
  "ja415:9.1_0": "“Sohaṁ tadeva punappunaṁ,",
  "ja415:9.2_0": "Vaṭumaṁ ācarissāmi sobhane;",
  "ja415:9.3_0": "Ariyācaritaṁ sukosale,",
  "ja415:9.4_0": "Arahanto me manāpāva passituṁ.",
  "ja415:10.1_0": "Devī viya accharūpamā,",
  "ja415:10.2_0": "Majjhe nārigaṇassa sobhasi;",
  "ja415:10.3_0": "Kiṁ kammamakāsi bhaddakaṁ,",
  "ja415:10.4_0": "Kenāsi vaṇṇavatī sukosale”.",
  "ja415:11.1_0": "“Ambaṭṭhakulassa khattiya,",
  "ja415:11.2_0": "Dāsyāhaṁ parapesiyā ahuṁ;",
  "ja415:11.3_0": "Saññatā ca dhammajīvinī,",
  "ja415:11.4_0": "Sīlavatī ca apāpadassanā.",
  "ja415:12.1_0": "Uddhaṭabhattaṁ ahaṁ tadā,",
  "ja415:12.2_0": "Caramānassa adāsi bhikkhuno;",
  "ja415:12.3_0": "Vittā sumanā sayaṁ ahaṁ,",
  "ja415:12.4_0": "Tassa kammassa phalaṁ mamedisan”ti.",
  "ja415:13.1_0": "Kummāsapiṇḍijātakaṁ dasamaṁ."
}