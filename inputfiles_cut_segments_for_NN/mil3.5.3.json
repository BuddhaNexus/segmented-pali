{
  "mil3.5.3:0.1_0": "Milindapañha",
  "mil3.5.3:0.2_0": "Buddhavagga",
  "mil3.5.3:0.3_0": "3. Buddhassaanuttarabhāvajānanapañha",
  "mil3.5.3:1.1_0": "Rājā āha—",
  "mil3.5.3:1.2_0": "“bhante nāgasena, sakkā jānituṁ ‘buddho anuttaro’”ti?",
  "mil3.5.3:1.3_0": "“Āma, mahārāja, sakkā jānituṁ ‘bhagavā anuttaro’”ti.",
  "mil3.5.3:1.4_0": "“Kathaṁ, bhante nāgasena, sakkā jānituṁ ‘buddho anuttaro’”ti?",
  "mil3.5.3:1.5_0": "“Bhūtapubbaṁ, mahārāja,",
  "mil3.5.3:1.5_1": "tissatthero nāma lekhācariyo ahosi,",
  "mil3.5.3:1.5_2": "bahūni vassāni abbhatītāni kālaṅkatassa kathaṁ so ñāyatī”ti.",
  "mil3.5.3:1.6_0": "“Lekhena, bhante”ti.",
  "mil3.5.3:1.7_0": "“Evameva kho, mahārāja,",
  "mil3.5.3:1.7_1": "yo dhammaṁ passati,",
  "mil3.5.3:1.7_2": "so bhagavantaṁ passati,",
  "mil3.5.3:1.7_3": "dhammo hi, mahārāja,",
  "mil3.5.3:1.7_4": "bhagavatā desito”ti.",
  "mil3.5.3:2.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.5.3:3.1_0": "Buddhassa anuttarabhāvajānanapañho tatiyo."
}