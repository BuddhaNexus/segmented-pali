{
  "vv16:0.1_0": "Vimānavatthu",
  "vv16:0.2_0": "Itthivimāna",
  "vv16:0.3_0": "Pīṭhavagga",
  "vv16:0.4_0": "16. Sirimāvimānavatthu",
  "vv16:1.1_0": "“Yuttā ca te paramaalaṅkatā hayā,",
  "vv16:1.2_0": "Adhomukhā aghasigamā balī javā;",
  "vv16:1.3_0": "Abhinimmitā pañcarathāsatā ca te,",
  "vv16:1.4_0": "Anventi taṁ sārathicoditā hayā.",
  "vv16:2.1_0": "Sā tiṭṭhasi rathavare alaṅkatā,",
  "vv16:2.2_0": "Obhāsayaṁ jalamiva joti pāvako;",
  "vv16:2.3_0": "Pucchāmi taṁ varatanu anomadassane,",
  "vv16:2.4_0": "Kasmā nu kāyā anadhivaraṁ upāgami”.",
  "vv16:3.1_0": "“Kāmaggapattānaṁ yamāhunuttaraṁ,",
  "vv16:3.2_0": "Nimmāya nimmāya ramanti devatā;",
  "vv16:3.3_0": "Tasmā kāyā accharā kāmavaṇṇinī,",
  "vv16:3.4_0": "Idhāgatā anadhivaraṁ namassituṁ”.",
  "vv16:4.1_0": "“Kiṁ tvaṁ pure sucaritamācarīdha,",
  "vv16:4.2_0": "Kenacchasi tvaṁ amitayasā sukhedhitā;",
  "vv16:4.3_0": "Iddhī ca te anadhivarā vihaṅgamā,",
  "vv16:4.4_0": "Vaṇṇo ca te dasa disā virocati.",
  "vv16:5.1_0": "Devehi tvaṁ parivutā sakkatā casi,",
  "vv16:5.2_0": "Kuto cutā sugatigatāsi devate;",
  "vv16:5.3_0": "Kassa vā tvaṁ vacanakarānusāsaniṁ,",
  "vv16:5.4_0": "Ācikkha me tvaṁ yadi buddhasāvikā”ti.",
  "vv16:6.1_0": "“Nagantare nagaravare sumāpite,",
  "vv16:6.2_0": "Paricārikā rājavarassa sirimato;",
  "vv16:6.3_0": "Nacce gīte paramasusikkhitā ahuṁ,",
  "vv16:6.4_0": "Sirimāti maṁ rājagahe avediṁsu.",
  "vv16:7.1_0": "Buddho ca me isinisabho vināyako,",
  "vv16:7.2_0": "Adesayī samudayadukkhaniccataṁ;",
  "vv16:7.3_0": "Asaṅkhataṁ dukkhanirodhasassataṁ,",
  "vv16:7.4_0": "Maggañcimaṁ akuṭilamañjasaṁ sivaṁ.",
  "vv16:8.1_0": "Sutvānahaṁ amatapadaṁ asaṅkhataṁ,",
  "vv16:8.2_0": "Tathāgatassanadhivarassa sāsanaṁ;",
  "vv16:8.3_0": "Sīlesvahaṁ paramasusaṁvutā ahuṁ,",
  "vv16:8.4_0": "Dhamme ṭhitā naravarabuddhadesite.",
  "vv16:9.1_0": "Ñatvānahaṁ virajapadaṁ asaṅkhataṁ,",
  "vv16:9.2_0": "Tathāgatenanadhivarena desitaṁ;",
  "vv16:9.3_0": "Tatthevahaṁ samathasamādhimāphusiṁ,",
  "vv16:9.4_0": "Sāyeva me paramaniyāmatā ahu.",
  "vv16:10.1_0": "Laddhānahaṁ amatavaraṁ visesanaṁ,",
  "vv16:10.2_0": "Ekaṁsikā abhisamaye visesiya;",
  "vv16:10.3_0": "Asaṁsayā bahujanapūjitā ahaṁ,",
  "vv16:10.4_0": "Khiḍḍāratiṁ paccanubhomanappakaṁ.",
  "vv16:11.1_0": "Evaṁ ahaṁ amatadasamhi devatā,",
  "vv16:11.2_0": "Tathāgatassanadhivarassa sāvikā;",
  "vv16:11.3_0": "Dhammaddasā paṭhamaphale patiṭṭhitā,",
  "vv16:11.4_0": "Sotāpannā na ca pana matthi duggati.",
  "vv16:12.1_0": "Sā vandituṁ anadhivaraṁ upāgamiṁ,",
  "vv16:12.2_0": "Pāsādike kusalarate ca bhikkhavo;",
  "vv16:12.3_0": "Namassituṁ samaṇasamāgamaṁ sivaṁ,",
  "vv16:12.4_0": "Sagāravā sirimato dhammarājino.",
  "vv16:13.1_0": "Disvā muniṁ muditamanamhi pīṇitā,",
  "vv16:13.2_0": "Tathāgataṁ naravaradammasārathiṁ;",
  "vv16:13.3_0": "Taṇhacchidaṁ kusalarataṁ vināyakaṁ,",
  "vv16:13.4_0": "Vandāmahaṁ paramahitānukampakan”ti.",
  "vv16:14.1_0": "Sirimāvimānaṁ soḷasamaṁ."
}