{
  "ja47:0.1_0": "Jātaka",
  "ja47:0.2_0": "Ekakanipāta",
  "ja47:0.3_0": "Atthakāmavagga",
  "ja47:0.4_0": "7. Vāruṇidūsakajātaka",
  "ja47:1.1_0": "“Na ve anatthakusalena,",
  "ja47:1.2_0": "atthacariyā sukhāvahā;",
  "ja47:1.3_0": "Hāpeti atthaṁ dummedho,",
  "ja47:1.4_0": "koṇḍañño vāruṇiṁ yathā”ti.",
  "ja47:2.1_0": "Vāruṇidūsakajātakaṁ sattamaṁ."
}