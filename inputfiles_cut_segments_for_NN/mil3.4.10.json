{
  "mil3.4.10:0.1_0": "Milindapañha",
  "mil3.4.10:0.2_0": "Nibbānavagga",
  "mil3.4.10:0.3_0": "10. Nibbānasukhajānanapañha",
  "mil3.4.10:1.1_0": "Rājā āha—",
  "mil3.4.10:1.2_0": "“bhante nāgasena, yo na labhati nibbānaṁ,",
  "mil3.4.10:1.2_1": "jānāti so ‘sukhaṁ nibbānan’”ti?",
  "mil3.4.10:1.3_0": "“Āma, mahārāja, yo na labhati nibbānaṁ,",
  "mil3.4.10:1.3_1": "jānāti so ‘sukhaṁ nibbānan’”ti.",
  "mil3.4.10:1.4_0": "“Kathaṁ, bhante nāgasena,",
  "mil3.4.10:1.4_1": "alabhanto jānāti ‘sukhaṁ nibbānan’”ti?",
  "mil3.4.10:1.5_0": "“Taṁ kiṁ maññasi, mahārāja, yesaṁ nacchinnā hatthapādā, jāneyyuṁ te, mahārāja,",
  "mil3.4.10:1.5_1": "‘dukkhaṁ hatthapādacchedanan’”ti?",
  "mil3.4.10:1.6_0": "“Āma, bhante, jāneyyun”ti.",
  "mil3.4.10:1.7_0": "“Kathaṁ jāneyyun”ti?",
  "mil3.4.10:1.8_0": "“Aññesaṁ, bhante,",
  "mil3.4.10:1.8_1": "chinnahatthapādānaṁ paridevitasaddaṁ sutvā jānanti ‘dukkhaṁ hatthapādacchedanan’”ti.",
  "mil3.4.10:1.9_0": "“Evameva kho, mahārāja, yesaṁ diṭṭhaṁ nibbānaṁ,",
  "mil3.4.10:1.9_1": "tesaṁ saddaṁ sutvā jānāti ‘sukhaṁ nibbānan’”ti.",
  "mil3.4.10:2.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.4.10:3.1_0": "Nibbānasukhajānanapañho dasamo.",
  "mil3.4.10:4.1_0": "Nibbānavaggo catuttho.",
  "mil3.4.10:5.1_0": "Imasmiṁ vagge dasa pañhā."
}