{
  "mil3.6.4:0.1_0": "Milindapañha",
  "mil3.6.4:0.2_0": "Sativagga",
  "mil3.6.4:0.3_0": "4. Bhagavatobrahmacāripañha",
  "mil3.6.4:1.1_0": "Rājā āha—",
  "mil3.6.4:1.2_0": "“bhante nāgasena, buddho brahmacārī”ti?",
  "mil3.6.4:1.3_0": "“Āma, mahārāja, bhagavā brahmacārī”ti.",
  "mil3.6.4:1.4_0": "“Tena hi, bhante nāgasena, buddho brahmuno sisso”ti?",
  "mil3.6.4:1.5_0": "“Atthi pana te, mahārāja, hatthipāmokkho”ti?",
  "mil3.6.4:1.6_0": "“Atthi, bhante”ti.",
  "mil3.6.4:1.7_0": "“Kiṁ nu kho, mahārāja,",
  "mil3.6.4:1.7_1": "so hatthī kadāci karahaci koñcanādaṁ nadatī”ti?",
  "mil3.6.4:1.8_0": "“Āma, bhante, nadatī”ti.",
  "mil3.6.4:1.9_0": "“Tena hi, mahārāja,",
  "mil3.6.4:1.9_1": "so hatthī koñcasakuṇassa sisso”ti?",
  "mil3.6.4:1.10_0": "“Na hi, bhante”ti.",
  "mil3.6.4:1.11_0": "“Kiṁ pana, mahārāja,",
  "mil3.6.4:1.11_1": "brahmā sabuddhiko abuddhiko”ti?",
  "mil3.6.4:1.12_0": "“Sabuddhiko, bhante”ti.",
  "mil3.6.4:1.13_0": "“Tena hi, mahārāja, brahmā bhagavato sisso”ti.",
  "mil3.6.4:2.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.6.4:3.1_0": "Bhagavato brahmacāripañho catuttho."
}