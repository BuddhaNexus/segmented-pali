{
  "tha-ap345:0.1_0": "Therāpadāna",
  "tha-ap345:0.2_0": "Ekapadumiyavagga",
  "tha-ap345:0.3_0": "3. Dhajadāyakattheraapadāna",
  "tha-ap345:1.1_0": "“Tisso nāma ahu satthā,",
  "tha-ap345:1.2_0": "lokajeṭṭho narāsabho;",
  "tha-ap345:1.3_0": "Tayopadhikkhaye disvā,",
  "tha-ap345:1.4_0": "dhajaṁ āropitaṁ mayā.",
  "tha-ap345:2.1_0": "Tena kammena sukatena,",
  "tha-ap345:2.2_0": "cetanāpaṇidhīhi ca;",
  "tha-ap345:2.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "tha-ap345:2.4_0": "tāvatiṁsamagacchahaṁ.",
  "tha-ap345:3.1_0": "Satānaṁ tīṇikkhattuñca,",
  "tha-ap345:3.2_0": "devarajjaṁ akārayiṁ;",
  "tha-ap345:3.3_0": "Satānaṁ pañcakkhattuñca,",
  "tha-ap345:3.4_0": "cakkavattī ahosahaṁ.",
  "tha-ap345:4.1_0": "Padesarajjaṁ vipulaṁ,",
  "tha-ap345:4.2_0": "gaṇanāto asaṅkhiyaṁ;",
  "tha-ap345:4.3_0": "Anubhomi sakaṁ kammaṁ,",
  "tha-ap345:4.4_0": "pubbe sukatamattano.",
  "tha-ap345:5.1_0": "Dvenavute ito kappe,",
  "tha-ap345:5.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap345:5.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap345:5.4_0": "dhajadānassidaṁ phalaṁ.",
  "tha-ap345:6.1_0": "Icchamāno cahaṁ ajja,",
  "tha-ap345:6.2_0": "sakānanaṁ sapabbataṁ;",
  "tha-ap345:6.3_0": "Khomadussena chādeyyaṁ,",
  "tha-ap345:6.4_0": "tadā mayhaṁ kate phalaṁ.",
  "tha-ap345:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap345:7.2_0": "…pe…",
  "tha-ap345:7.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap345:8.1_0": "Itthaṁ sudaṁ āyasmā dhajadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap345:9.1_0": "Dhajadāyakattherassāpadānaṁ tatiyaṁ."
}