{
  "vv54:0.1_0": "Vimānavatthu",
  "vv54:0.2_0": "Purisavimāna",
  "vv54:0.3_0": "Mahārathavagga",
  "vv54:0.4_0": "4. Kakkaṭakarasadāyakavimānavatthu",
  "vv54:1.1_0": "“Uccamidaṁ maṇithūṇaṁ vimānaṁ,",
  "vv54:1.2_0": "Samantato dvādasa yojanāni;",
  "vv54:1.3_0": "Kūṭāgārā sattasatā uḷārā,",
  "vv54:1.4_0": "Veḷuriyathambhā rucakatthatā subhā.",
  "vv54:2.1_0": "Tatthacchasi pivasi khādasi ca,",
  "vv54:2.2_0": "Dibbā ca vīṇā pavadanti vagguṁ;",
  "vv54:2.3_0": "Dibbā rasā kāmaguṇettha pañca,",
  "vv54:2.4_0": "Nāriyo ca naccanti suvaṇṇachannā.",
  "vv54:3.1_0": "Kena tetādiso vaṇṇo,",
  "vv54:3.2_0": "kena te idha mijjhati;",
  "vv54:3.3_0": "Uppajjanti ca te bhogā,",
  "vv54:3.4_0": "ye keci manaso piyā.",
  "vv54:4.1_0": "Pucchāmi taṁ deva mahānubhāva,",
  "vv54:4.2_0": "Manussabhūto kimakāsi puññaṁ;",
  "vv54:4.3_0": "Kenāsi evaṁ jalitānubhāvo,",
  "vv54:4.4_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv54:5.1_0": "So devaputto attamano,",
  "vv54:5.2_0": "moggallānena pucchito;",
  "vv54:5.3_0": "Pañhaṁ puṭṭho viyākāsi,",
  "vv54:5.4_0": "yassa kammassidaṁ phalaṁ.",
  "vv54:6.1_0": "“Satisamuppādakaro,",
  "vv54:6.2_0": "dvāre kakkaṭako ṭhito;",
  "vv54:6.3_0": "Niṭṭhito jātarūpassa,",
  "vv54:6.4_0": "sobhati dasapādako.",
  "vv54:7.1_0": "Tena metādiso vaṇṇo,",
  "vv54:7.2_0": "tena me idha mijjhati;",
  "vv54:7.3_0": "Uppajjanti ca me bhogā,",
  "vv54:7.4_0": "ye keci manaso piyā.",
  "vv54:8.1_0": "Akkhāmi te bhikkhu mahānubhāva,",
  "vv54:8.2_0": "Manussabhūto yamakāsi puññaṁ;",
  "vv54:8.3_0": "Tenamhi evaṁ jalitānubhāvo,",
  "vv54:8.4_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv54:9.1_0": "Kakkaṭakarasadāyakavimānaṁ catutthaṁ.",
  "vv54:10.1_0": "(Anantaraṁ pañcavimānaṁ yathā kakkaṭakarasadāyakavimānaṁ tathā vitthāretabbaṁ.)"
}