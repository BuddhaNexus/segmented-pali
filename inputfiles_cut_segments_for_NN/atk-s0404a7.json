{
  "atk-s0404a7:0_0": "(8) 3. Yamakavaggo",
  "atk-s0404a7:1_0": "1-2. Saddhāsuttadvayavaṇṇanā",
  "atk-s0404a7:2_0": "71-72.",
  "atk-s0404a7:2_1": "Aṭṭhamassa paṭhame no ca sīlavāti na sīlesu paripūrakārī.",
  "atk-s0404a7:2_2": "Samantapāsādikoti samantato pasādajanako.",
  "atk-s0404a7:2_3": "Sabbākāraparipūroti sabbehi samaṇākārehi samaṇadhammakoṭṭhāsehi paripūro.",
  "atk-s0404a7:2_4": "Dutiye santāti paccanīkasantatāya santā.",
  "atk-s0404a7:2_5": "Vimokkhāti paccanīkadhammehi vimuttattā ca vimokkhā.",
  "atk-s0404a7:3_0": "3-9. Maraṇassatisuttadvayādivaṇṇanā",
  "atk-s0404a7:4_0": "73-79.",
  "atk-s0404a7:4_1": "Tatiye bhāvetha noti bhāvetha nu.",
  "atk-s0404a7:4_2": "Sāsananti anusiṭṭhi.",
  "atk-s0404a7:4_3": "Āsavānaṁ khayāyāti arahattaphalatthāya.",
  "atk-s0404a7:4_4": "Catutthe patihitāyāti paṭipannāya.",
  "atk-s0404a7:4_5": "So mamassa antarāyoti so mama jīvitantarāyopi,",
  "atk-s0404a7:4_6": "puthujjanakālakiriyaṁ karontassa saggantarāyopi maggantarāyopi assa.",
  "atk-s0404a7:4_7": "Satthakā vā me vātāti satthaṁ viya aṅgamaṅgāni kantantīti satthakā.",
  "atk-s0404a7:4_8": "Pañcamādīni vuttanayāneva.",
  "atk-s0404a7:4_9": "Navame saṁsaggārāmatāti pañcavidhe saṁsagge ārāmatā.",
  "atk-s0404a7:5_0": "10. Kusītārambhavatthusuttavaṇṇanā",
  "atk-s0404a7:6_0": "80.",
  "atk-s0404a7:6_1": "Dasame kusītavatthūnīti kusītassa alasassa vatthūni patiṭṭhā,",
  "atk-s0404a7:6_2": "kosajjakāraṇānīti attho.",
  "atk-s0404a7:6_3": "Kammaṁ kattabbaṁ hotīti cīvaravicāraṇādikammaṁ kattabbaṁ hoti.",
  "atk-s0404a7:6_4": "Na vīriyaṁ ārabhatīti duvidhampi vīriyaṁ nārabhati.",
  "atk-s0404a7:6_5": "Appattassāti jhānavipassanāmaggaphaladhammassa appattassa pattiyā.",
  "atk-s0404a7:6_6": "Anadhigatassāti tasseva anadhigatassa adhigamatthāya.",
  "atk-s0404a7:6_7": "Asacchikatassāti tadeva asacchikatassa sacchikaraṇatthāya.",
  "atk-s0404a7:6_8": "Idaṁ paṭhamanti idaṁ “handāhaṁ nipajjāmī”ti evaṁ osīdanaṁ paṭhamaṁ kusītavatthu.",
  "atk-s0404a7:6_9": "Iminā nayena sabbattha attho veditabbo.",
  "atk-s0404a7:6_10": "Māsācitakaṁ maññeti ettha pana māsācitaṁ nāma tintamāso.",
  "atk-s0404a7:6_11": "Yathā tintamāso garuko hoti,",
  "atk-s0404a7:6_12": "evaṁ garukoti adhippāyo.",
  "atk-s0404a7:6_13": "Gilānā vuṭṭhito hotīti gilāno hutvā pacchā vuṭṭhito hoti.",
  "atk-s0404a7:6_14": "Ārambhavatthūnīti vīriyakāraṇāni.",
  "atk-s0404a7:6_15": "Tesampi imināva nayena attho veditabbo.",
  "atk-s0404a7:6_16": "Sesaṁ sabbattha uttānamevāti.",
  "atk-s0404a7:7_0": "Yamakavaggo aṭṭhamo."
}