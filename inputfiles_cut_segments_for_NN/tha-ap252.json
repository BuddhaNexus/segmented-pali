{
  "tha-ap252:0.1_0": "Therāpadāna",
  "tha-ap252:0.2_0": "Tuvaradāyakavagga",
  "tha-ap252:0.3_0": "10 Uddālakadāyakattheraapadāna",
  "tha-ap252:1.1_0": "“Kakudho nāma nāmena,",
  "tha-ap252:1.2_0": "sayambhū aparājito;",
  "tha-ap252:1.3_0": "Pavanā nikkhamitvāna,",
  "tha-ap252:1.4_0": "anuppatto mahānadiṁ.",
  "tha-ap252:2.1_0": "Uddālakaṁ gahetvāna,",
  "tha-ap252:2.2_0": "sayambhussa adāsahaṁ;",
  "tha-ap252:2.3_0": "Saṁyatassujubhūtassa,",
  "tha-ap252:2.4_0": "pasannamānaso ahaṁ.",
  "tha-ap252:3.1_0": "Ekattiṁse ito kappe,",
  "tha-ap252:3.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap252:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap252:3.4_0": "pupphadānassidaṁ phalaṁ.",
  "tha-ap252:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap252:4.2_0": "…pe…",
  "tha-ap252:4.3_0": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap252:5.1_0": "Itthaṁ sudaṁ āyasmā uddālakadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap252:6.1_0": "Uddālakadāyakattherassāpadānaṁ dasamaṁ.",
  "tha-ap252:7.1_0": "Tuvaradāyakavaggo pañcavīsatimo.",
  "tha-ap252:8.1_0": "Tassuddānaṁ",
  "tha-ap252:9.1_0": "Tuvaranāganaḷinā,",
  "tha-ap252:9.2_0": "viravī kuṭidhūpako;",
  "tha-ap252:9.3_0": "Patto dhātusattaliyo,",
  "tha-ap252:9.4_0": "bimbi uddālakena ca;",
  "tha-ap252:9.5_0": "Sattatiṁsati gāthāyo,",
  "tha-ap252:9.6_0": "gaṇitāyo vibhāvibhi."
}