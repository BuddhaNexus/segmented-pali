{
  "iti40:0.1_0": "Itivuttaka 40",
  "iti40:0.2_0": "Dukanipāta",
  "iti40:0.3_0": "Dutiyavagga",
  "iti40:0.4_0": "Vijjāsutta",
  "iti40:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti40:2.1_0": "“Avijjā, bhikkhave,",
  "iti40:2.1_1": "pubbaṅgamā akusalānaṁ dhammānaṁ samāpattiyā anvadeva ahirikaṁ anottappaṁ;",
  "iti40:2.2_0": "vijjā ca kho, bhikkhave,",
  "iti40:2.2_1": "pubbaṅgamā kusalānaṁ dhammānaṁ samāpattiyā anvadeva hirottappan”ti.",
  "iti40:2.3_0": "Etamatthaṁ bhagavā avoca.",
  "iti40:2.4_0": "Tatthetaṁ iti vuccati:",
  "iti40:3.1_0": "“Yā kācimā duggatiyo,",
  "iti40:3.2_0": "asmiṁ loke paramhi ca;",
  "iti40:3.3_0": "Avijjāmūlikā sabbā,",
  "iti40:3.4_0": "icchālobhasamussayā.",
  "iti40:4.1_0": "Yato ca hoti pāpiccho,",
  "iti40:4.2_0": "ahirīko anādaro;",
  "iti40:4.3_0": "Tato pāpaṁ pasavati,",
  "iti40:4.4_0": "apāyaṁ tena gacchati.",
  "iti40:5.1_0": "Tasmā chandañca lobhañca,",
  "iti40:5.2_0": "avijjañca virājayaṁ;",
  "iti40:5.3_0": "Vijjaṁ uppādayaṁ bhikkhu,",
  "iti40:5.4_0": "sabbā duggatiyo jahe”ti.",
  "iti40:6.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti40:6.2_0": "Tatiyaṁ.",
  "iti40:6.3_0": "Paṭhamabhāṇavāro."
}