{
  "mil7.7.7:0.1_0": "Milindapañha",
  "mil7.7.7:0.2_0": "Opammakathāpañha",
  "mil7.7.7:0.3_0": "Makkaṭakavagga",
  "mil7.7.7:0.4_0": "7. Maṇiratanaṅgapañha",
  "mil7.7.7:1.1_0": "“Bhante nāgasena,",
  "mil7.7.7:1.1_1": "‘maṇiratanassa tīṇi aṅgāni gahetabbānī’ti yaṁ vadesi,",
  "mil7.7.7:1.1_2": "katamāni tāni tīṇi aṅgāni gahetabbānī”ti?",
  "mil7.7.7:1.2_0": "“Yathā, mahārāja, maṇiratanaṁ ekantaparisuddhaṁ;",
  "mil7.7.7:1.3_0": "evameva kho, mahārāja,",
  "mil7.7.7:1.3_1": "yoginā yogāvacarena ekantaparisuddhājīvena bhavitabbaṁ.",
  "mil7.7.7:1.4_0": "Idaṁ, mahārāja,",
  "mil7.7.7:1.4_1": "maṇiratanassa paṭhamaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.7.7:2.1_0": "Puna caparaṁ, mahārāja,",
  "mil7.7.7:2.1_1": "maṇiratanaṁ na kenaci saddhiṁ missīyati;",
  "mil7.7.7:2.2_0": "evameva kho, mahārāja,",
  "mil7.7.7:2.2_1": "yoginā yogāvacarena pāpehi pāpasahāyehi saddhiṁ na missitabbaṁ.",
  "mil7.7.7:2.3_0": "Idaṁ, mahārāja,",
  "mil7.7.7:2.3_1": "maṇiratanassa dutiyaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.7.7:3.1_0": "Puna caparaṁ, mahārāja,",
  "mil7.7.7:3.1_1": "maṇiratanaṁ jātiratanehi yojīyati;",
  "mil7.7.7:3.2_0": "evameva kho, mahārāja,",
  "mil7.7.7:3.2_1": "yoginā yogāvacarena uttamavarajātimantehi saddhiṁ saṁvasitabbaṁ,",
  "mil7.7.7:3.2_2": "paṭipannakaphalaṭṭhasekkhaphalasamaṅgīhi sotāpannasakadāgāmianāgāmiarahantatevijjachaḷabhiññasamaṇamaṇiratanehi saddhiṁ saṁvasitabbaṁ.",
  "mil7.7.7:3.3_0": "Idaṁ, mahārāja,",
  "mil7.7.7:3.3_1": "maṇiratanassa tatiyaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.7.7:3.4_0": "Bhāsitampetaṁ, mahārāja,",
  "mil7.7.7:3.4_1": "bhagavatā devātidevena suttanipāte—",
  "mil7.7.7:4.1_0": "‘Suddhā suddhehi saṁvāsaṁ,",
  "mil7.7.7:4.2_0": "Kappayavho patissatā;",
  "mil7.7.7:4.3_0": "Tato samaggā nipakā,",
  "mil7.7.7:4.4_0": "Dukkhassantaṁ karissathā’”ti.",
  "mil7.7.7:5.1_0": "Maṇiratanaṅgapañho sattamo."
}