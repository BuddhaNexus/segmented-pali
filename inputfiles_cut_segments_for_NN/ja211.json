{
  "ja211:0.1_0": "Jātaka",
  "ja211:0.2_0": "Dukanipāta",
  "ja211:0.3_0": "Bīraṇathambhavagga",
  "ja211:0.4_0": "1. Somadattajātaka",
  "ja211:1.1_0": "“Akāsi yoggaṁ dhuvamappamatto,",
  "ja211:1.2_0": "Saṁvaccharaṁ bīraṇathambhakasmiṁ;",
  "ja211:1.3_0": "Byākāsi saññaṁ parisaṁ vigayha,",
  "ja211:1.4_0": "Na niyyamo tāyati appapaññaṁ”.",
  "ja211:2.1_0": "“Dvayaṁ yācanako tāta,",
  "ja211:2.2_0": "somadatta nigacchati;",
  "ja211:2.3_0": "Alābhaṁ dhanalābhaṁ vā,",
  "ja211:2.4_0": "evaṁ dhammā hi yācanā”ti.",
  "ja211:3.1_0": "Somadattajātakaṁ paṭhamaṁ."
}