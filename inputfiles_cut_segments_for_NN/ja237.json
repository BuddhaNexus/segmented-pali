{
  "ja237:0.1_0": "Jātaka",
  "ja237:0.2_0": "Dukanipāta",
  "ja237:0.3_0": "Upāhanavagga",
  "ja237:0.4_0": "7. Sāketajātaka",
  "ja237:1.1_0": "“Ko nu kho bhagavā hetu,",
  "ja237:1.2_0": "ekacce idha puggale;",
  "ja237:1.3_0": "Atīva hadayaṁ nibbāti,",
  "ja237:1.4_0": "cittañcāpi pasīdati”.",
  "ja237:2.1_0": "“Pubbeva sannivāsena,",
  "ja237:2.2_0": "paccuppannahitena vā;",
  "ja237:2.3_0": "Evaṁ taṁ jāyate pemaṁ,",
  "ja237:2.4_0": "uppalaṁva yathodake”ti.",
  "ja237:3.1_0": "Sāketajātakaṁ sattamaṁ."
}