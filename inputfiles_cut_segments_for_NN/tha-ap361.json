{
  "tha-ap361:0.1_0": "Therāpadāna",
  "tha-ap361:0.2_0": "Saddasaññakavagga",
  "tha-ap361:0.3_0": "9 Saraṇagamaniyattheraapadāna",
  "tha-ap361:1.1_0": "“Āruhimha tadā nāvaṁ,",
  "tha-ap361:1.2_0": "bhikkhu cājīvako cahaṁ;",
  "tha-ap361:1.3_0": "Nāvāya bhijjamānāya,",
  "tha-ap361:1.4_0": "bhikkhu me saraṇaṁ adā.",
  "tha-ap361:2.1_0": "Ekattiṁse ito kappe,",
  "tha-ap361:2.2_0": "yañca me saraṇaṁ adā;",
  "tha-ap361:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap361:2.4_0": "saraṇāgamane phalaṁ.",
  "tha-ap361:3.1_0": "Paṭisambhidā catasso,",
  "tha-ap361:3.2_0": "…pe…",
  "tha-ap361:3.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap361:4.1_0": "Itthaṁ sudaṁ āyasmā saraṇagamaniyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap361:5.1_0": "Saraṇagamaniyattherassāpadānaṁ navamaṁ."
}