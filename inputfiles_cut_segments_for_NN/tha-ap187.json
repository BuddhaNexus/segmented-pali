{
  "tha-ap187:0.1_0": "Therāpadāna",
  "tha-ap187:0.2_0": "Kuṭajapupphiyavagga",
  "tha-ap187:0.3_0": "5. Isimuggadāyakattheraapadāna",
  "tha-ap187:1.1_0": "“Udentaṁ sataraṁsiṁva,",
  "tha-ap187:1.2_0": "pītaraṁsiṁva bhāṇumaṁ;",
  "tha-ap187:1.3_0": "Kakudhaṁ vilasantaṁva,",
  "tha-ap187:1.4_0": "padumuttaranāyakaṁ.",
  "tha-ap187:2.1_0": "Isimuggāni pisitvā,",
  "tha-ap187:2.2_0": "madhukhudde anīḷake;",
  "tha-ap187:2.3_0": "Pāsādeva ṭhito santo,",
  "tha-ap187:2.4_0": "adāsiṁ lokabandhuno.",
  "tha-ap187:3.1_0": "Aṭṭhasatasahassāni,",
  "tha-ap187:3.2_0": "ahesuṁ buddhasāvakā;",
  "tha-ap187:3.3_0": "Sabbesaṁ pattapūrentaṁ,",
  "tha-ap187:3.4_0": "tato cāpi bahuttaraṁ.",
  "tha-ap187:4.1_0": "Tena cittappasādena,",
  "tha-ap187:4.2_0": "sukkamūlena codito;",
  "tha-ap187:4.3_0": "Kappānaṁ satasahassaṁ,",
  "tha-ap187:4.4_0": "duggatiṁ nupapajjahaṁ.",
  "tha-ap187:5.1_0": "Cattālīsamhi sahasse,",
  "tha-ap187:5.2_0": "kappānaṁ aṭṭhatiṁsa te;",
  "tha-ap187:5.3_0": "Isimuggasanāmā te,",
  "tha-ap187:5.4_0": "cakkavattī mahabbalā.",
  "tha-ap187:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap187:6.2_0": "…pe…",
  "tha-ap187:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap187:7.1_0": "Itthaṁ sudaṁ āyasmā isimuggadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap187:8.1_0": "Isimuggadāyakattherassāpadānaṁ pañcamaṁ."
}