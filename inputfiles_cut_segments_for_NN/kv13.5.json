{
  "kv13.5:0.1_0": "Kathāvatthu",
  "kv13.5:0.2_0": "Mahāpaṇṇāsaka",
  "kv13.5:0.3_0": "Terasamavagga",
  "kv13.5:0.4_0": "Nivutakathā",
  "kv13.5:1.1_0": "Nivuto nīvaraṇaṁ jahatīti?",
  "kv13.5:1.2_0": "Āmantā.",
  "kv13.5:1.3_0": "Ratto rāgaṁ jahati, duṭṭho dosaṁ jahati, mūḷho mohaṁ jahati, kiliṭṭho kilese jahatīti?",
  "kv13.5:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv13.5:1.5_0": "rāgena rāgaṁ jahati, dosena dosaṁ jahati, mohena mohaṁ jahati, kilesehi kilese jahatīti?",
  "kv13.5:1.6_0": "Na hevaṁ vattabbe …pe….",
  "kv13.5:2.1_0": "Rāgo cittasampayutto, maggo cittasampayuttoti?",
  "kv13.5:2.2_0": "Āmantā.",
  "kv13.5:2.3_0": "Dvinnaṁ phassānaṁ …pe…",
  "kv13.5:2.4_0": "dvinnaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv13.5:2.5_0": "Na hevaṁ vattabbe …pe…",
  "kv13.5:2.6_0": "rāgo akusalo, maggo kusaloti?",
  "kv13.5:2.7_0": "Āmantā.",
  "kv13.5:2.8_0": "Kusalākusalā sāvajjānavajjā hīnapaṇītā kaṇhasukkasappaṭibhāgā dhammā sammukhībhāvaṁ āgacchantīti?",
  "kv13.5:2.9_0": "Na hevaṁ vattabbe …pe….",
  "kv13.5:3.1_0": "Kusalākusalā sāvajjānavajjā hīnapaṇītā kaṇhasukkasappaṭibhāgā dhammā sammukhībhāvaṁ āgacchantīti?",
  "kv13.5:3.2_0": "Āmantā.",
  "kv13.5:3.3_0": "Nanu vuttaṁ bhagavatā—",
  "kv13.5:3.4_0": "“cattārimāni, bhikkhave, suvidūravidūrāni.",
  "kv13.5:3.5_0": "Katamāni cattāri?",
  "kv13.5:3.6_0": "Nabhañca, bhikkhave, pathavī ca—",
  "kv13.5:3.7_0": "idaṁ paṭhamaṁ suvidūravidūraṁ …pe… tasmā sataṁ dhammo asabbhi ārakā”ti.",
  "kv13.5:3.8_0": "Attheva suttantoti?",
  "kv13.5:3.9_0": "Āmantā.",
  "kv13.5:3.10_0": "Tena hi na vattabbaṁ—",
  "kv13.5:3.11_0": "“kusalākusalā …pe…",
  "kv13.5:3.12_0": "sammukhībhāvaṁ āgacchantī”ti.",
  "kv13.5:4.1_0": "Nivuto nīvaraṇaṁ jahatīti?",
  "kv13.5:4.2_0": "Āmantā.",
  "kv13.5:4.3_0": "Nanu vuttaṁ bhagavatā—",
  "kv13.5:4.4_0": "“so evaṁ samāhite citte parisuddhe pariyodāte anaṅgaṇe vigatūpakkilese mudubhūte kammaniye ṭhite āneñjappatte āsavānaṁ khayañāṇāya cittaṁ abhininnāmetī”ti.",
  "kv13.5:4.5_0": "Attheva suttantoti?",
  "kv13.5:4.6_0": "Āmantā.",
  "kv13.5:4.7_0": "Tena hi na vattabbaṁ—",
  "kv13.5:4.8_0": "“nivuto nīvaraṇaṁ jahatī”ti …pe….",
  "kv13.5:5.1_0": "Na vattabbaṁ—",
  "kv13.5:5.2_0": "“nivuto nīvaraṇaṁ jahatī”ti?",
  "kv13.5:5.3_0": "Āmantā.",
  "kv13.5:5.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv13.5:5.5_0": "“tassa evaṁ jānato evaṁ passato kāmāsavāpi cittaṁ vimuccati …pe…",
  "kv13.5:5.6_0": "avijjāsavāpi cittaṁ vimuccatī”ti.",
  "kv13.5:5.7_0": "Attheva suttantoti?",
  "kv13.5:5.8_0": "Āmantā.",
  "kv13.5:5.9_0": "Tena hi nivuto nīvaraṇaṁ jahatīti.",
  "kv13.5:5.10_0": "Nivutakathā niṭṭhitā."
}