{
  "tha-ap29:0.1_0": "Therāpadāna",
  "tha-ap29:0.2_0": "Subhūtivagga",
  "tha-ap29:0.3_0": "7 Pulinapūjakattheraapadāna",
  "tha-ap29:1.1_0": "“Vipassissa bhagavato,",
  "tha-ap29:1.2_0": "bodhiyā pādaputtame;",
  "tha-ap29:1.3_0": "Purāṇapulinaṁ hitvā,",
  "tha-ap29:1.4_0": "suddhapulinamākiriṁ.",
  "tha-ap29:2.1_0": "Ekanavutito kappe,",
  "tha-ap29:2.2_0": "yaṁ pulinamadāsahaṁ;",
  "tha-ap29:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap29:2.4_0": "pulinadānassidaṁ phalaṁ.",
  "tha-ap29:3.1_0": "Tiṁsatime ito kappe,",
  "tha-ap29:3.2_0": "rājā āsiṁ janādhibhū;",
  "tha-ap29:3.3_0": "Mahāpulinanāmena,",
  "tha-ap29:3.4_0": "cakkavattī mahabbalo.",
  "tha-ap29:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap29:4.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap29:4.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap29:4.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap29:5.1_0": "Itthaṁ sudaṁ āyasmā pulinapūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap29:6.1_0": "Pulinapūjakattherassāpadānaṁ sattamaṁ."
}