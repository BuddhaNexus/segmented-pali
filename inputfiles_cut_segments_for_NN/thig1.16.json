{
  "thig1.16:0.1_0": "Therīgāthā",
  "thig1.16:0.2_0": "Ekakanipāta",
  "thig1.16:0.3_0": "16 Vuḍḍhapabbajitasumanātherīgāthā",
  "thig1.16:1.1_0": "“Sukhaṁ tvaṁ vuḍḍhike sehi,",
  "thig1.16:1.2_0": "katvā coḷena pārutā;",
  "thig1.16:1.3_0": "Upasanto hi te rāgo,",
  "thig1.16:1.4_0": "sītibhūtāsi nibbutā”ti.",
  "thig1.16:2.1_0": "… Sumanā vuḍḍhapabbajitā therī …."
}