{
  "thag1.8:0.1_0": "Theragāthā",
  "thag1.8:0.2_0": "Ekakanipāta",
  "thag1.8:0.3_0": "Paṭhamavagga",
  "thag1.8:0.4_0": "1.8. Vīrattheragāthā",
  "thag1.8:1.1_0": "“Yo duddamiyo damena danto,",
  "thag1.8:1.2_0": "Vīro santusito vitiṇṇakaṅkho;",
  "thag1.8:1.3_0": "Vijitāvī apetalomahaṁso,",
  "thag1.8:1.4_0": "Vīro so parinibbuto ṭhitatto”ti.",
  "thag1.8:1.5_0": "Itthaṁ sudaṁ āyasmā vīro thero gāthaṁ abhāsitthāti."
}