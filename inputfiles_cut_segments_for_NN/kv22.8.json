{
  "kv22.8:0.1_0": "Kathāvatthu",
  "kv22.8:0.2_0": "Mahāpaṇṇāsaka",
  "kv22.8:0.3_0": "Bāvīsatimavagga",
  "kv22.8:0.4_0": "Khaṇikakathā",
  "kv22.8:1.1_0": "Ekacittakkhaṇikā sabbe dhammāti?",
  "kv22.8:1.2_0": "Āmantā.",
  "kv22.8:1.3_0": "Citte mahāpathavī saṇṭhāti,",
  "kv22.8:1.3_1": "mahāsamuddo saṇṭhāti,",
  "kv22.8:1.3_2": "sinerupabbatarājā saṇṭhāti,",
  "kv22.8:1.3_3": "āpo saṇṭhāti,",
  "kv22.8:1.3_4": "tejo saṇṭhāti,",
  "kv22.8:1.3_5": "vāyo saṇṭhāti,",
  "kv22.8:1.3_6": "tiṇakaṭṭhavanappatayo saṇṭhahantīti?",
  "kv22.8:1.4_0": "Na hevaṁ vattabbe …pe….",
  "kv22.8:2.1_0": "Ekacittakkhaṇikā sabbe dhammāti?",
  "kv22.8:2.2_0": "Āmantā.",
  "kv22.8:2.3_0": "Cakkhāyatanaṁ cakkhuviññāṇena sahajātanti?",
  "kv22.8:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv22.8:2.5_0": "cakkhāyatanaṁ cakkhuviññāṇena sahajātanti?",
  "kv22.8:2.6_0": "Āmantā.",
  "kv22.8:2.7_0": "Nanu āyasmā sāriputto etadavoca—",
  "kv22.8:2.8_0": "“ajjhattikañceva, āvuso,",
  "kv22.8:2.8_1": "cakkhuṁ aparibhinnaṁ hoti,",
  "kv22.8:2.8_2": "bāhirā ca rūpā na āpāthaṁ āgacchanti,",
  "kv22.8:2.8_3": "no ca tajjo samannāhāro hoti,",
  "kv22.8:2.8_4": "neva tāva tajjassa viññāṇabhāgassa pātubhāvo hoti.",
  "kv22.8:2.9_0": "Ajjhattikañceva, āvuso,",
  "kv22.8:2.9_1": "cakkhuṁ aparibhinnaṁ hoti,",
  "kv22.8:2.9_2": "bāhirā ca rūpā āpāthaṁ āgacchanti,",
  "kv22.8:2.9_3": "no ca tajjo samannāhāro hoti,",
  "kv22.8:2.9_4": "neva tāva tajjassa viññāṇabhāgassa pātubhāvo hoti.",
  "kv22.8:2.10_0": "Yato ca kho, āvuso,",
  "kv22.8:2.10_1": "ajjhattikañceva cakkhuṁ aparibhinnaṁ hoti,",
  "kv22.8:2.10_2": "bāhirā ca rūpā āpāthaṁ āgacchanti,",
  "kv22.8:2.10_3": "tajjo ca samannāhāro hoti,",
  "kv22.8:2.10_4": "evaṁ tajjassa viññāṇabhāgassa pātubhāvo hotī”ti.",
  "kv22.8:2.11_0": "Attheva suttantoti?",
  "kv22.8:2.12_0": "Āmantā.",
  "kv22.8:2.13_0": "Tena hi na vattabbaṁ—",
  "kv22.8:2.14_0": "“cakkhāyatanaṁ cakkhuviññāṇena sahajātan”ti.",
  "kv22.8:2.15_0": "(…)",
  "kv22.8:3.1_0": "Sotāyatanaṁ …pe…",
  "kv22.8:3.2_0": "ghānāyatanaṁ …pe…",
  "kv22.8:3.3_0": "jivhāyatanaṁ …pe…",
  "kv22.8:3.4_0": "kāyāyatanaṁ kāyaviññāṇena sahajātanti?",
  "kv22.8:3.5_0": "Na hevaṁ vattabbe …pe…",
  "kv22.8:3.6_0": "kāyāyatanaṁ kāyaviññāṇena sahajātanti?",
  "kv22.8:3.7_0": "Āmantā.",
  "kv22.8:3.8_0": "Nanu āyasmā sāriputto etadavoca—",
  "kv22.8:3.9_0": "“ajjhattiko ceva, āvuso,",
  "kv22.8:3.9_1": "kāyo aparibhinno hoti,",
  "kv22.8:3.9_2": "bāhirā ca phoṭṭhabbā na āpāthaṁ āgacchanti,",
  "kv22.8:3.9_3": "no ca …pe…",
  "kv22.8:3.10_0": "ajjhattiko ceva, āvuso, kāyo aparibhinno hoti, bāhirā ca phoṭṭhabbā āpāthaṁ āgacchanti, no ca …pe…",
  "kv22.8:3.11_0": "yato ca kho, āvuso,",
  "kv22.8:3.11_1": "ajjhattiko ceva kāyo aparibhinno hoti,",
  "kv22.8:3.11_2": "bāhirā ca phoṭṭhabbā āpāthaṁ āgacchanti,",
  "kv22.8:3.11_3": "tajjo ca samannāhāro hoti,",
  "kv22.8:3.11_4": "evaṁ tajjassa viññāṇabhāgassa pātubhāvo hotī”ti.",
  "kv22.8:3.12_0": "Attheva suttantoti?",
  "kv22.8:3.13_0": "Āmantā.",
  "kv22.8:3.14_0": "Tena hi na vattabbaṁ—",
  "kv22.8:3.15_0": "“kāyāyatanaṁ kāyaviññāṇena sahajātan”ti.",
  "kv22.8:4.1_0": "Na vattabbaṁ—",
  "kv22.8:4.2_0": "“ekacittakkhaṇikā sabbe dhammā”ti?",
  "kv22.8:4.3_0": "Āmantā.",
  "kv22.8:4.4_0": "Sabbe dhammā niccā dhuvā sassatā avipariṇāmadhammāti?",
  "kv22.8:4.5_0": "Na hevaṁ vattabbe.",
  "kv22.8:4.6_0": "Tena hi ekacittakkhaṇikā sabbe dhammāti.",
  "kv22.8:4.7_0": "Khaṇikakathā niṭṭhitā.",
  "kv22.8:4.8_0": "Bāvīsatimo vaggo.",
  "kv22.8:4.9_0": "Tassuddānaṁ",
  "kv22.8:5.1_0": "Atthi kiñci saṁyojanaṁ appahāya parinibbānaṁ,",
  "kv22.8:5.2_0": "arahā kusalacitto parinibbāyati,",
  "kv22.8:5.3_0": "arahā āneñje ṭhito parinibbāyati,",
  "kv22.8:5.4_0": "atthi gabbhaseyyāya dhammābhisamayo,",
  "kv22.8:5.5_0": "atthi gabbhaseyyāya arahattappatti,",
  "kv22.8:5.6_0": "atthi supinagatassa dhammābhisamayo,",
  "kv22.8:5.7_0": "atthi supinagatassa arahattappatti,",
  "kv22.8:5.8_0": "sabbaṁ supinagatassa cittaṁ abyākataṁ,",
  "kv22.8:5.9_0": "natthi kāci āsevanapaccayatā,",
  "kv22.8:5.10_0": "ekacittakkhaṇikā sabbe dhammāti."
}