{
  "tha-ap346:0.1_0": "Therāpadāna",
  "tha-ap346:0.2_0": "Ekapadumiyavagga",
  "tha-ap346:0.3_0": "4 Tikiṅkaṇipūjakattheraapadāna",
  "tha-ap346:1.1_0": "“Himavantassāvidūre,",
  "tha-ap346:1.2_0": "bhūtagaṇo nāma pabbato;",
  "tha-ap346:1.3_0": "Tatthaddasaṁ paṁsukūlaṁ,",
  "tha-ap346:1.4_0": "dumaggamhi vilaggitaṁ.",
  "tha-ap346:2.1_0": "Tīṇi kiṅkaṇipupphāni,",
  "tha-ap346:2.2_0": "ocinitvānahaṁ tadā;",
  "tha-ap346:2.3_0": "Haṭṭho haṭṭhena cittena,",
  "tha-ap346:2.4_0": "paṁsukūlaṁ apūjayiṁ.",
  "tha-ap346:3.1_0": "Ekattiṁse ito kappe,",
  "tha-ap346:3.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap346:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap346:3.4_0": "tiṇṇaṁ pupphānidaṁ phalaṁ.",
  "tha-ap346:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap346:4.2_0": "…pe…",
  "tha-ap346:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap346:5.1_0": "Itthaṁ sudaṁ āyasmā tikiṅkaṇipūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap346:6.1_0": "Tikiṅkaṇipūjakattherassāpadānaṁ catutthaṁ."
}