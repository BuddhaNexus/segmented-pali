{
  "mil3.5.9:0.1_0": "Milindapañha",
  "mil3.5.9:0.2_0": "Buddhavagga",
  "mil3.5.9:0.3_0": "9. Uppajjatijānanapañha",
  "mil3.5.9:1.1_0": "Rājā āha—",
  "mil3.5.9:1.2_0": "“bhante nāgasena, yo uppajjati, jānāti so ‘uppajjissāmī’”ti?",
  "mil3.5.9:1.3_0": "“Āma, mahārāja,",
  "mil3.5.9:1.3_1": "yo uppajjati jānāti so ‘uppajjissāmī’”ti.",
  "mil3.5.9:2.1_0": "“Opammaṁ karohī”ti.",
  "mil3.5.9:2.2_0": "“Yathā, mahārāja,",
  "mil3.5.9:2.2_1": "kassako gahapatiko bījāni pathaviyaṁ nikkhipitvā sammā deve vassante jānāti ‘dhaññaṁ nibbattissatī’”ti?",
  "mil3.5.9:2.3_0": "“Āma, bhante, jāneyyā”ti.",
  "mil3.5.9:2.4_0": "“Evameva kho, mahārāja, yo uppajjati, jānāti so ‘uppajjissāmī’”ti.",
  "mil3.5.9:3.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.5.9:4.1_0": "Uppajjatijānanapañho navamo."
}