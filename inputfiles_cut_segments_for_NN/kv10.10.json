{
  "kv10.10:0.1_0": "Kathāvatthu",
  "kv10.10:0.2_0": "Mahāpaṇṇāsaka",
  "kv10.10:0.3_0": "Dasamavagga",
  "kv10.10:0.4_0": "Viññattisīlantikathā",
  "kv10.10:1.1_0": "Viññatti sīlanti?",
  "kv10.10:1.2_0": "Āmantā.",
  "kv10.10:1.3_0": "Pāṇātipātā veramaṇīti?",
  "kv10.10:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv10.10:1.5_0": "adinnādānā veramaṇīti?",
  "kv10.10:1.6_0": "Na hevaṁ vattabbe …pe…",
  "kv10.10:1.7_0": "kāmesumicchācārā veramaṇīti?",
  "kv10.10:1.8_0": "Na hevaṁ vattabbe …pe…",
  "kv10.10:1.9_0": "musāvādā veramaṇīti?",
  "kv10.10:1.10_0": "Na hevaṁ vattabbe …pe…",
  "kv10.10:1.11_0": "surāmerayamajjapamādaṭṭhānā veramaṇīti?",
  "kv10.10:1.12_0": "Na hevaṁ vattabbe …pe….",
  "kv10.10:2.1_0": "Abhivādanaṁ sīlaṁ,",
  "kv10.10:2.1_1": "paccuṭṭhānaṁ sīlaṁ,",
  "kv10.10:2.1_2": "añjalikammaṁ sīlaṁ,",
  "kv10.10:2.1_3": "sāmīcikammaṁ sīlaṁ,",
  "kv10.10:2.1_4": "āsanābhihāro sīlaṁ,",
  "kv10.10:2.1_5": "seyyābhihāro sīlaṁ,",
  "kv10.10:2.1_6": "pādodakābhihāro sīlaṁ,",
  "kv10.10:2.1_7": "pādakathalikābhihāro sīlaṁ,",
  "kv10.10:2.1_8": "nhāne piṭṭhiparikammaṁ sīlanti?",
  "kv10.10:2.2_0": "Āmantā.",
  "kv10.10:2.3_0": "Pāṇātipātā veramaṇīti?",
  "kv10.10:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv10.10:2.5_0": "surāmerayamajjapamādaṭṭhānā veramaṇīti?",
  "kv10.10:2.6_0": "Na hevaṁ vattabbe …pe….",
  "kv10.10:3.1_0": "Na vattabbaṁ—",
  "kv10.10:3.2_0": "“viññatti sīlan”ti?",
  "kv10.10:3.3_0": "Āmantā.",
  "kv10.10:3.4_0": "Dussilyanti?",
  "kv10.10:3.5_0": "Na hevaṁ vattabbe …pe…",
  "kv10.10:3.6_0": "tena hi viññatti sīlanti.",
  "kv10.10:3.7_0": "Viññatti sīlantikathā niṭṭhitā."
}