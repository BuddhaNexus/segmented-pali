{
  "vv59:0.1_0": "Vimānavatthu",
  "vv59:0.2_0": "Purisavimāna",
  "vv59:0.3_0": "Mahārathavagga",
  "vv59:0.4_0": "9. Dutiyasūcivimānavatthu",
  "vv59:1.1_0": "“Uccamidaṁ maṇithūṇaṁ vimānaṁ,",
  "vv59:1.2_0": "Samantato dvādasa yojanāni;",
  "vv59:1.3_0": "Kūṭāgārā sattasatā uḷārā,",
  "vv59:1.4_0": "Veḷuriyathambhā rucakatthatā subhā.",
  "vv59:2.1_0": "Tatthacchasi pivasi khādasi ca,",
  "vv59:2.2_0": "Dibbā ca vīṇā pavadanti vagguṁ;",
  "vv59:2.3_0": "Dibbā rasā kāmaguṇettha pañca,",
  "vv59:2.4_0": "Nāriyo ca naccanti suvaṇṇachannā.",
  "vv59:3.1_0": "Kena tetādiso vaṇṇo,",
  "vv59:3.2_0": "…pe…",
  "vv59:3.3_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv59:4.1_0": "So devaputto attamano,",
  "vv59:4.2_0": "…pe…",
  "vv59:4.3_0": "yassa kammassidaṁ phalaṁ.",
  "vv59:5.1_0": "“Ahaṁ manussesu manussabhūto,",
  "vv59:5.2_0": "Purimajātiyā manussaloke.",
  "vv59:6.1_0": "Addasaṁ virajaṁ bhikkhuṁ,",
  "vv59:6.2_0": "vippasannamanāvilaṁ;",
  "vv59:6.3_0": "Tassa adāsahaṁ sūciṁ,",
  "vv59:6.4_0": "pasanno sehi pāṇibhi.",
  "vv59:7.1_0": "Tena metādiso vaṇṇo,",
  "vv59:7.2_0": "…pe…",
  "vv59:7.3_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv59:8.1_0": "Dutiyasūcivimānaṁ navamaṁ."
}