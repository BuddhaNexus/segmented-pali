{
  "kv16.7:0.1_0": "Kathāvatthu",
  "kv16.7:0.2_0": "Mahāpaṇṇāsaka",
  "kv16.7:0.3_0": "Soḷasamavagga",
  "kv16.7:0.4_0": "Rūpaṅkusalākusalantikathā",
  "kv16.7:1.1_0": "Rūpaṁ kusalanti?",
  "kv16.7:1.2_0": "Āmantā.",
  "kv16.7:1.3_0": "Sārammaṇaṁ, atthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv16.7:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv16.7:1.5_0": "nanu anārammaṇaṁ,",
  "kv16.7:1.5_1": "natthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv16.7:1.6_0": "Āmantā.",
  "kv16.7:1.7_0": "Hañci anārammaṇaṁ,",
  "kv16.7:1.7_1": "natthi tassa āvaṭṭanā …pe… paṇidhi, no ca vata re vattabbe—",
  "kv16.7:1.8_0": "“rūpaṁ kusalan”ti.",
  "kv16.7:2.1_0": "Alobho kusalo sārammaṇo,",
  "kv16.7:2.1_1": "atthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv16.7:2.2_0": "Āmantā.",
  "kv16.7:2.3_0": "Rūpaṁ kusalaṁ sārammaṇaṁ,",
  "kv16.7:2.3_1": "atthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv16.7:2.4_0": "Na hevaṁ vattabbe …pe….",
  "kv16.7:3.1_0": "Adoso kusalo …pe…",
  "kv16.7:3.2_0": "amoho kusalo …pe…",
  "kv16.7:3.3_0": "saddhā …",
  "kv16.7:3.4_0": "vīriyaṁ …",
  "kv16.7:3.5_0": "sati …",
  "kv16.7:3.6_0": "samādhi …pe…",
  "kv16.7:3.7_0": "paññā kusalā sārammaṇā,",
  "kv16.7:3.7_1": "atthi tāya āvaṭṭanā …pe… paṇidhīti?",
  "kv16.7:3.8_0": "Āmantā.",
  "kv16.7:3.9_0": "Rūpaṁ kusalaṁ sārammaṇaṁ,",
  "kv16.7:3.9_1": "atthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv16.7:3.10_0": "Na hevaṁ vattabbe …pe….",
  "kv16.7:4.1_0": "Rūpaṁ kusalaṁ anārammaṇaṁ,",
  "kv16.7:4.1_1": "natthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv16.7:4.2_0": "Āmantā.",
  "kv16.7:4.3_0": "Alobho kusalo anārammaṇo,",
  "kv16.7:4.3_1": "natthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv16.7:4.4_0": "Na hevaṁ vattabbe …pe….",
  "kv16.7:5.1_0": "Rūpaṁ kusalaṁ anārammaṇaṁ,",
  "kv16.7:5.1_1": "natthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv16.7:5.2_0": "Āmantā.",
  "kv16.7:5.3_0": "Adoso kusalo …pe…",
  "kv16.7:5.4_0": "paññā kusalā anārammaṇā,",
  "kv16.7:5.4_1": "natthi tāya āvaṭṭanā …pe… paṇidhīti?",
  "kv16.7:5.5_0": "Na hevaṁ vattabbe …pe….",
  "kv16.7:6.1_0": "Rūpaṁ akusalanti?",
  "kv16.7:6.2_0": "Āmantā.",
  "kv16.7:6.3_0": "Sārammaṇaṁ, atthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv16.7:6.4_0": "Na hevaṁ vattabbe …pe…",
  "kv16.7:6.5_0": "nanu anārammaṇaṁ,",
  "kv16.7:6.5_1": "natthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv16.7:6.6_0": "Āmantā.",
  "kv16.7:6.7_0": "Hañci anārammaṇaṁ,",
  "kv16.7:6.7_1": "natthi tassa āvaṭṭanā …pe… paṇidhi, no ca vata re vattabbe—",
  "kv16.7:6.8_0": "“rūpaṁ akusalan”ti …pe….",
  "kv16.7:7.1_0": "Lobho akusalo sārammaṇo,",
  "kv16.7:7.1_1": "atthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv16.7:7.2_0": "Āmantā.",
  "kv16.7:7.3_0": "Rūpaṁ akusalaṁ sārammaṇaṁ,",
  "kv16.7:7.3_1": "atthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv16.7:7.4_0": "Na hevaṁ vattabbe …pe…",
  "kv16.7:7.5_0": "doso …",
  "kv16.7:7.6_0": "moho …",
  "kv16.7:7.7_0": "māno …pe…",
  "kv16.7:7.8_0": "anottappaṁ akusalaṁ sārammaṇaṁ,",
  "kv16.7:7.8_1": "atthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv16.7:7.9_0": "Āmantā.",
  "kv16.7:7.10_0": "Rūpaṁ akusalaṁ sārammaṇaṁ,",
  "kv16.7:7.10_1": "atthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv16.7:7.11_0": "Na hevaṁ vattabbe …pe….",
  "kv16.7:8.1_0": "Rūpaṁ akusalaṁ anārammaṇaṁ,",
  "kv16.7:8.1_1": "natthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv16.7:8.2_0": "Āmantā.",
  "kv16.7:8.3_0": "Lobho akusalo anārammaṇo,",
  "kv16.7:8.3_1": "natthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv16.7:8.4_0": "Na hevaṁ vattabbe …pe…",
  "kv16.7:8.5_0": "rūpaṁ akusalaṁ anārammaṇaṁ,",
  "kv16.7:8.5_1": "natthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv16.7:8.6_0": "Āmantā.",
  "kv16.7:8.7_0": "Doso …",
  "kv16.7:8.8_0": "moho …pe…",
  "kv16.7:8.9_0": "anottappaṁ akusalaṁ anārammaṇaṁ,",
  "kv16.7:8.9_1": "natthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv16.7:8.10_0": "Na hevaṁ vattabbe …pe….",
  "kv16.7:9.1_0": "Na vattabbaṁ—",
  "kv16.7:9.2_0": "“rūpaṁ kusalampi akusalampī”ti?",
  "kv16.7:9.3_0": "Āmantā.",
  "kv16.7:9.4_0": "Nanu kāyakammaṁ vacīkammaṁ kusalampi akusalampīti?",
  "kv16.7:9.5_0": "Āmantā.",
  "kv16.7:9.6_0": "Hañci kāyakammaṁ vacīkammaṁ kusalampi akusalampi, tena vata re vattabbe—",
  "kv16.7:9.7_0": "“rūpaṁ kusalampi akusalampī”ti.",
  "kv16.7:9.8_0": "Rūpaṁ kusalākusalantikathā niṭṭhitā."
}