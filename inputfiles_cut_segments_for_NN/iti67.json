{
  "iti67:0.1_0": "Itivuttaka 67",
  "iti67:0.2_0": "Tikanipāta",
  "iti67:0.3_0": "Dutiyavagga",
  "iti67:0.4_0": "Moneyyasutta",
  "iti67:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti67:2.1_0": "“Tīṇimāni, bhikkhave, moneyyāni.",
  "iti67:2.2_0": "Katamāni tīṇi?",
  "iti67:2.3_0": "Kāyamoneyyaṁ, vacīmoneyyaṁ, manomoneyyaṁ—",
  "iti67:2.4_0": "imāni kho, bhikkhave, tīṇi moneyyānī”ti.",
  "iti67:2.5_0": "Etamatthaṁ bhagavā avoca.",
  "iti67:2.6_0": "Tatthetaṁ iti vuccati:",
  "iti67:3.1_0": "“Kāyamuniṁ vacīmuniṁ,",
  "iti67:3.2_0": "manomunimanāsavaṁ;",
  "iti67:3.3_0": "Muniṁ moneyyasampannaṁ,",
  "iti67:3.4_0": "āhu ninhātapāpakan”ti.",
  "iti67:4.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti67:4.2_0": "Aṭṭhamaṁ."
}