{
  "ja370:0.1_0": "Jātaka",
  "ja370:0.2_0": "Pañcakanipāta",
  "ja370:0.3_0": "Vaṇṇārohavagga",
  "ja370:0.4_0": "10. Palāsajātaka",
  "ja370:1.1_0": "“Haṁso palāsamavaca,",
  "ja370:1.2_0": "nigrodho samma jāyati;",
  "ja370:1.3_0": "Aṅkasmiṁ te nisinnova,",
  "ja370:1.4_0": "so te mammāni checchati”.",
  "ja370:2.1_0": "“Vaḍḍhatāmeva nigrodho,",
  "ja370:2.2_0": "patiṭṭhassa bhavāmahaṁ;",
  "ja370:2.3_0": "Yathā pitā ca mātā ca,",
  "ja370:2.4_0": "evaṁ me so bhavissati”.",
  "ja370:3.1_0": "“Yaṁ tvaṁ aṅkasmiṁ vaḍḍhesi,",
  "ja370:3.2_0": "khīrarukkhaṁ bhayānakaṁ;",
  "ja370:3.3_0": "Āmanta kho taṁ gacchāma,",
  "ja370:3.4_0": "vuḍḍhi massa na ruccati”.",
  "ja370:4.1_0": "“Idāni kho maṁ bhāyeti,",
  "ja370:4.2_0": "mahānerunidassanaṁ;",
  "ja370:4.3_0": "Haṁsassa anabhiññāya,",
  "ja370:4.4_0": "mahā me bhayamāgataṁ”.",
  "ja370:5.1_0": "“Na tassa vuḍḍhi kusalappasatthā,",
  "ja370:5.2_0": "Yo vaḍḍhamāno ghasate patiṭṭhaṁ;",
  "ja370:5.3_0": "Tassūparodhaṁ parisaṅkamāno,",
  "ja370:5.4_0": "Patārayī mūlavadhāya dhīro”ti.",
  "ja370:6.1_0": "Palāsajātakaṁ dasamaṁ.",
  "ja370:6.2_0": "Vaṇṇārohavaggo dutiyo."
}