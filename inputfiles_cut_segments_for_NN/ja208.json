{
  "ja208:0.1_0": "Jātaka",
  "ja208:0.2_0": "Dukanipāta",
  "ja208:0.3_0": "Nataṁdaḷhavagga",
  "ja208:0.4_0": "8. Susumārajātaka",
  "ja208:1.1_0": "“Alaṁ metehi ambehi,",
  "ja208:1.2_0": "jambūhi panasehi ca;",
  "ja208:1.3_0": "Yāni pāraṁ samuddassa,",
  "ja208:1.4_0": "varaṁ mayhaṁ udumbaro.",
  "ja208:2.1_0": "Mahatī vata te bondi,",
  "ja208:2.2_0": "na ca paññā tadūpikā;",
  "ja208:2.3_0": "Susumāra vañcito mesi,",
  "ja208:2.4_0": "gaccha dāni yathāsukhan”ti.",
  "ja208:3.1_0": "Susumārajātakaṁ aṭṭhamaṁ."
}