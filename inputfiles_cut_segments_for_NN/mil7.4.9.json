{
  "mil7.4.9:0.1_0": "Milindapañha",
  "mil7.4.9:0.2_0": "Opammakathāpañha",
  "mil7.4.9:0.3_0": "Pathavīvagga",
  "mil7.4.9:0.4_0": "9. Sakkaṅgapañha",
  "mil7.4.9:1.1_0": "“Bhante nāgasena,",
  "mil7.4.9:1.1_1": "‘sakkassa tīṇi aṅgāni gahetabbānī’ti yaṁ vadesi,",
  "mil7.4.9:1.1_2": "katamāni tāni tīṇi aṅgāni gahetabbānī”ti?",
  "mil7.4.9:1.2_0": "“Yathā, mahārāja, sakko ekantasukhasamappito;",
  "mil7.4.9:1.3_0": "evameva kho, mahārāja,",
  "mil7.4.9:1.3_1": "yoginā yogāvacarena ekantapavivekasukhābhiratena bhavitabbaṁ.",
  "mil7.4.9:1.4_0": "Idaṁ, mahārāja,",
  "mil7.4.9:1.4_1": "sakkassa paṭhamaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.4.9:2.1_0": "Puna caparaṁ, mahārāja, sakko deve disvā paggaṇhāti, hāsamabhijaneti;",
  "mil7.4.9:2.2_0": "evameva kho, mahārāja,",
  "mil7.4.9:2.2_1": "yoginā yogāvacarena kusalesu dhammesu alīnamatanditaṁ santaṁ mānasaṁ paggahetabbaṁ, hāsamabhijanetabbaṁ,",
  "mil7.4.9:2.2_2": "uṭṭhahitabbaṁ ghaṭitabbaṁ vāyamitabbaṁ.",
  "mil7.4.9:2.3_0": "Idaṁ, mahārāja, sakkassa dutiyaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.4.9:3.1_0": "Puna caparaṁ, mahārāja,",
  "mil7.4.9:3.1_1": "sakkassa anabhirati nuppajjati;",
  "mil7.4.9:3.2_0": "evameva kho, mahārāja,",
  "mil7.4.9:3.2_1": "yoginā yogāvacarena suññāgāre anabhirati na uppādetabbā.",
  "mil7.4.9:3.3_0": "Idaṁ, mahārāja, sakkassa tatiyaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.4.9:3.4_0": "Bhāsitampetaṁ, mahārāja, therena subhūtinā—",
  "mil7.4.9:4.1_0": "‘Sāsane te mahāvīra,",
  "mil7.4.9:4.2_0": "yato pabbajito ahaṁ;",
  "mil7.4.9:4.3_0": "Nābhijānāmi uppannaṁ,",
  "mil7.4.9:4.4_0": "mānasaṁ kāmasaṁhitan’”ti.",
  "mil7.4.9:5.1_0": "Sakkaṅgapañho navamo."
}