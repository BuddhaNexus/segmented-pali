{
  "ja31:0.1_0": "Jātaka",
  "ja31:0.2_0": "Ekakanipāta",
  "ja31:0.3_0": "Kulāvakavagga",
  "ja31:0.4_0": "1. Kulāvakajātaka",
  "ja31:1.1_0": "“Kulāvakā mātali simbalismiṁ,",
  "ja31:1.2_0": "Īsāmukhena parivajjayassu;",
  "ja31:1.3_0": "Kāmaṁ cajāma asuresu pāṇaṁ,",
  "ja31:1.4_0": "Māme dijā vikkulāvā ahesun”ti.",
  "ja31:2.1_0": "Kulāvakajātakaṁ paṭhamaṁ."
}