{
  "tha-ap110:0.1_0": "Therāpadāna",
  "tha-ap110:0.2_0": "Bhikkhadāyivagga",
  "tha-ap110:0.3_0": "8 Āsanupaṭṭhāhakattheraapadāna",
  "tha-ap110:1.1_0": "“Kānanaṁ vanamogayha,",
  "tha-ap110:1.2_0": "appasaddaṁ nirākulaṁ;",
  "tha-ap110:1.3_0": "Sīhāsanaṁ mayā dinnaṁ,",
  "tha-ap110:1.4_0": "atthadassissa tādino.",
  "tha-ap110:2.1_0": "Mālāhatthaṁ gahetvāna,",
  "tha-ap110:2.2_0": "katvā ca naṁ padakkhiṇaṁ;",
  "tha-ap110:2.3_0": "Satthāraṁ payirupāsitvā,",
  "tha-ap110:2.4_0": "pakkāmiṁ uttarāmukho.",
  "tha-ap110:3.1_0": "Tena kammena dvipadinda,",
  "tha-ap110:3.2_0": "lokajeṭṭha narāsabha;",
  "tha-ap110:3.3_0": "Sannibbāpemi attānaṁ,",
  "tha-ap110:3.4_0": "bhavā sabbe samūhatā.",
  "tha-ap110:4.1_0": "Aṭṭhārasakappasate,",
  "tha-ap110:4.2_0": "yaṁ dānamadadiṁ tadā;",
  "tha-ap110:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap110:4.4_0": "sīhāsanassidaṁ phalaṁ.",
  "tha-ap110:5.1_0": "Ito sattakappasate,",
  "tha-ap110:5.2_0": "sannibbāpaka khattiyo;",
  "tha-ap110:5.3_0": "Sattaratanasampanno,",
  "tha-ap110:5.4_0": "cakkavattī mahabbalo.",
  "tha-ap110:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap110:6.2_0": "…pe…",
  "tha-ap110:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap110:7.1_0": "Itthaṁ sudaṁ āyasmā āsanupaṭṭhāhako thero imā gāthāyo abhāsitthāti.",
  "tha-ap110:8.1_0": "Āsanupaṭṭhāhakattherassāpadānaṁ aṭṭhamaṁ."
}