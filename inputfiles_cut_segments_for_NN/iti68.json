{
  "iti68:0.1_0": "Itivuttaka 68",
  "iti68:0.2_0": "Tikanipāta",
  "iti68:0.3_0": "Dutiyavagga",
  "iti68:0.4_0": "Paṭhamarāgasutta",
  "iti68:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti68:2.1_0": "“Yassa kassaci, bhikkhave, rāgo appahīno, doso appahīno, moho appahīno—",
  "iti68:2.2_0": "ayaṁ vuccati, bhikkhave,",
  "iti68:2.2_1": "‘baddho mārassa paṭimukkassa mārapāso yathākāmakaraṇīyo pāpimato’.",
  "iti68:2.3_0": "Yassa kassaci, bhikkhave, rāgo pahīno, doso pahīno, moho pahīno—",
  "iti68:2.4_0": "ayaṁ vuccati, bhikkhave,",
  "iti68:2.4_1": "‘abaddho mārassa omukkassa mārapāso na yathākāmakaraṇīyo pāpimato’”ti.",
  "iti68:2.5_0": "Etamatthaṁ bhagavā avoca.",
  "iti68:2.6_0": "Tatthetaṁ iti vuccati:",
  "iti68:3.1_0": "“Yassa rāgo ca doso ca,",
  "iti68:3.2_0": "avijjā ca virājitā;",
  "iti68:3.3_0": "Taṁ bhāvitattaññataraṁ,",
  "iti68:3.4_0": "brahmabhūtaṁ tathāgataṁ;",
  "iti68:3.5_0": "Buddhaṁ verabhayātītaṁ,",
  "iti68:3.6_0": "āhu sabbappahāyinan”ti.",
  "iti68:4.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti68:4.2_0": "Navamaṁ."
}