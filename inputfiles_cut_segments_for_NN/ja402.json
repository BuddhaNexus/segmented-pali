{
  "ja402:0.1_0": "Jātaka",
  "ja402:0.2_0": "Sattakanipāta",
  "ja402:0.3_0": "Kukkuvagga",
  "ja402:0.4_0": "7. Sattubhastajātaka",
  "ja402:1.1_0": "“Vibbhantacitto kupitindriyosi,",
  "ja402:1.2_0": "Nettehi te vārigaṇā savanti;",
  "ja402:1.3_0": "Kiṁ te naṭṭhaṁ kiṁ pana patthayāno,",
  "ja402:1.4_0": "Idhāgamā brahme tadiṅgha brūhi”.",
  "ja402:2.1_0": "“Miyyetha bhariyā vajato mamajja,",
  "ja402:2.2_0": "Agacchato maraṇamāha yakkho;",
  "ja402:2.3_0": "Etena dukkhena pavedhitosmi,",
  "ja402:2.4_0": "Akkhāhi me senaka etamatthaṁ”.",
  "ja402:3.1_0": "“Bahūni ṭhānāni vicintayitvā,",
  "ja402:3.2_0": "Yamettha vakkhāmi tadeva saccaṁ;",
  "ja402:3.3_0": "Maññāmi te brāhmaṇa sattubhastaṁ,",
  "ja402:3.4_0": "Ajānato kaṇhasappo paviṭṭho.",
  "ja402:4.1_0": "Ādāya daṇḍaṁ parisumbha bhastaṁ,",
  "ja402:4.2_0": "Passeḷamūgaṁ uragaṁ dujivhaṁ;",
  "ja402:4.3_0": "Chindajja kaṅkhaṁ vicikicchitāni,",
  "ja402:4.4_0": "Bhujaṅgamaṁ passa pamuñca bhastaṁ”.",
  "ja402:5.1_0": "Saṁviggarūpo parisāya majjhe,",
  "ja402:5.2_0": "So brāhmaṇo sattubhastaṁ pamuñci;",
  "ja402:5.3_0": "Atha nikkhami urago uggatejo,",
  "ja402:5.4_0": "Āsīviso sappo phaṇaṁ karitvā.",
  "ja402:6.1_0": "“Suladdhalābhā janakassa rañño,",
  "ja402:6.2_0": "Yo passatī senakaṁ sādhupaññaṁ;",
  "ja402:6.3_0": "Vivaṭṭachaddo nusi sabbadassī,",
  "ja402:6.4_0": "Ñāṇaṁ nu te brāhmaṇa bhiṁsarūpaṁ”.",
  "ja402:7.1_0": "“Imāni me sattasatāni atthi,",
  "ja402:7.2_0": "Gaṇhāhi sabbāni dadāmi tuyhaṁ;",
  "ja402:7.3_0": "Tayā hi me jīvitamajja laddhaṁ,",
  "ja402:7.4_0": "Athopi bhariyāya makāsi sotthiṁ”.",
  "ja402:8.1_0": "“Na paṇḍitā vetanamādiyanti,",
  "ja402:8.2_0": "Citrāhi gāthāhi subhāsitāhi;",
  "ja402:8.3_0": "Itopi te brahme dadantu vittaṁ,",
  "ja402:8.4_0": "Ādāya tvaṁ gaccha sakaṁ niketan”ti.",
  "ja402:9.1_0": "Sattubhastajātakaṁ sattamaṁ."
}