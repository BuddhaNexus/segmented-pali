{
  "thi-ap25:0.1_0": "Therīapadāna",
  "thi-ap25:0.2_0": "Kuṇḍalakesīvagga",
  "thi-ap25:0.3_0": "5 Nandātherīapadāna",
  "thi-ap25:1.1_0": "“Padumuttaro nāma jino,",
  "thi-ap25:1.2_0": "sabbadhammāna pāragū;",
  "thi-ap25:1.3_0": "Ito satasahassamhi,",
  "thi-ap25:1.4_0": "kappe uppajji nāyako.",
  "thi-ap25:2.1_0": "Ovādako viññāpako,",
  "thi-ap25:2.2_0": "tārako sabbapāṇinaṁ;",
  "thi-ap25:2.3_0": "Desanākusalo buddho,",
  "thi-ap25:2.4_0": "tāresi janataṁ bahuṁ.",
  "thi-ap25:3.1_0": "Anukampako kāruṇiko,",
  "thi-ap25:3.2_0": "hitesī sabbapāṇinaṁ;",
  "thi-ap25:3.3_0": "Sampatte titthiye sabbe,",
  "thi-ap25:3.4_0": "pañcasīle patiṭṭhapi.",
  "thi-ap25:4.1_0": "Evaṁ nirākulaṁ āsi,",
  "thi-ap25:4.2_0": "suññataṁ titthiyehi ca;",
  "thi-ap25:4.3_0": "Vicittaṁ arahantehi,",
  "thi-ap25:4.4_0": "vasībhūtehi tādibhi.",
  "thi-ap25:5.1_0": "Ratanānaṭṭhapaññāsaṁ,",
  "thi-ap25:5.2_0": "uggatova mahāmuni;",
  "thi-ap25:5.3_0": "Kañcanagghiyasaṅkāso,",
  "thi-ap25:5.4_0": "bāttiṁsavaralakkhaṇo.",
  "thi-ap25:6.1_0": "Vassasatasahassāni,",
  "thi-ap25:6.2_0": "āyu vijjati tāvade;",
  "thi-ap25:6.3_0": "Tāvatā tiṭṭhamāno so,",
  "thi-ap25:6.4_0": "tāresi janataṁ bahuṁ.",
  "thi-ap25:7.1_0": "Tadāhaṁ haṁsavatiyaṁ,",
  "thi-ap25:7.2_0": "jātā seṭṭhikule ahuṁ;",
  "thi-ap25:7.3_0": "Nānāratanapajjote,",
  "thi-ap25:7.4_0": "mahāsukhasamappitā.",
  "thi-ap25:8.1_0": "Upetvā taṁ mahāvīraṁ,",
  "thi-ap25:8.2_0": "assosiṁ dhammadesanaṁ;",
  "thi-ap25:8.3_0": "Amataṁ paramassādaṁ,",
  "thi-ap25:8.4_0": "paramatthanivedakaṁ.",
  "thi-ap25:9.1_0": "Tadā nimantayitvāna,",
  "thi-ap25:9.2_0": "sasaṅghaṁ lokanāyakaṁ;",
  "thi-ap25:9.3_0": "Datvā tassa mahādānaṁ,",
  "thi-ap25:9.4_0": "pasannā sehi pāṇibhi.",
  "thi-ap25:10.1_0": "Jhāyinīnaṁ bhikkhunīnaṁ,",
  "thi-ap25:10.2_0": "aggaṭṭhānamapatthayiṁ;",
  "thi-ap25:10.3_0": "Nipacca sirasā dhīraṁ,",
  "thi-ap25:10.4_0": "sasaṅghaṁ lokanāyakaṁ.",
  "thi-ap25:11.1_0": "Tadā adantadamako,",
  "thi-ap25:11.2_0": "tilokasaraṇo pabhū;",
  "thi-ap25:11.3_0": "Byākāsi narasārathi,",
  "thi-ap25:11.4_0": "‘lacchase taṁ supatthitaṁ.",
  "thi-ap25:12.1_0": "Satasahassito kappe,",
  "thi-ap25:12.2_0": "okkākakulasambhavo;",
  "thi-ap25:12.3_0": "Gotamo nāma gottena,",
  "thi-ap25:12.4_0": "satthā loke bhavissati.",
  "thi-ap25:13.1_0": "Tassa dhammesu dāyādā,",
  "thi-ap25:13.2_0": "orasā dhammanimmitā;",
  "thi-ap25:13.3_0": "Nandāti nāma nāmena,",
  "thi-ap25:13.4_0": "hessati satthu sāvikā’.",
  "thi-ap25:14.1_0": "Taṁ sutvā muditā hutvā,",
  "thi-ap25:14.2_0": "yāvajīvaṁ tadā jinaṁ;",
  "thi-ap25:14.3_0": "Mettacittā paricariṁ,",
  "thi-ap25:14.4_0": "paccayehi vināyakaṁ.",
  "thi-ap25:15.1_0": "Tena kammena sukatena,",
  "thi-ap25:15.2_0": "cetanāpaṇidhīhi ca;",
  "thi-ap25:15.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "thi-ap25:15.4_0": "tāvatiṁsamagacchahaṁ.",
  "thi-ap25:16.1_0": "Tato cutā yāmamagaṁ,",
  "thi-ap25:16.2_0": "tatohaṁ tusitaṁ gatā;",
  "thi-ap25:16.3_0": "Tato ca nimmānaratiṁ,",
  "thi-ap25:16.4_0": "vasavattipuraṁ tato.",
  "thi-ap25:17.1_0": "Yattha yatthūpapajjāmi,",
  "thi-ap25:17.2_0": "tassa kammassa vāhasā;",
  "thi-ap25:17.3_0": "Tattha tattheva rājūnaṁ,",
  "thi-ap25:17.4_0": "mahesittamakārayiṁ.",
  "thi-ap25:18.1_0": "Tato cutā manussatte,",
  "thi-ap25:18.2_0": "rājānaṁ cakkavattinaṁ;",
  "thi-ap25:18.3_0": "Maṇḍalīnañca rājūnaṁ,",
  "thi-ap25:18.4_0": "mahesittamakārayiṁ.",
  "thi-ap25:19.1_0": "Sampattiṁ anubhotvāna,",
  "thi-ap25:19.2_0": "devesu manujesu ca;",
  "thi-ap25:19.3_0": "Sabbattha sukhitā hutvā,",
  "thi-ap25:19.4_0": "nekakappesu saṁsariṁ.",
  "thi-ap25:20.1_0": "Pacchime bhave sampatte,",
  "thi-ap25:20.2_0": "suramme kapilavhaye;",
  "thi-ap25:20.3_0": "Rañño suddhodanassāhaṁ,",
  "thi-ap25:20.4_0": "dhītā āsiṁ aninditā.",
  "thi-ap25:21.1_0": "Siriyā rūpiniṁ disvā,",
  "thi-ap25:21.2_0": "nanditaṁ āsi taṁ kulaṁ;",
  "thi-ap25:21.3_0": "Tena nandāti me nāmaṁ,",
  "thi-ap25:21.4_0": "sundaraṁ pavaraṁ ahu.",
  "thi-ap25:22.1_0": "Yuvatīnañca sabbāsaṁ,",
  "thi-ap25:22.2_0": "kalyāṇīti ca vissutā;",
  "thi-ap25:22.3_0": "Tasmimpi nagare ramme,",
  "thi-ap25:22.4_0": "ṭhapetvā taṁ yasodharaṁ.",
  "thi-ap25:23.1_0": "Jeṭṭho bhātā tilokaggo,",
  "thi-ap25:23.2_0": "pacchimo arahā tathā;",
  "thi-ap25:23.3_0": "Ekākinī gahaṭṭhāhaṁ,",
  "thi-ap25:23.4_0": "mātarā paricoditā.",
  "thi-ap25:24.1_0": "‘Sākiyamhi kule jātā,",
  "thi-ap25:24.2_0": "putte buddhānujā tuvaṁ;",
  "thi-ap25:24.3_0": "Nandenapi vinā bhūtā,",
  "thi-ap25:24.4_0": "agāre kiṁ nu acchasi.",
  "thi-ap25:25.1_0": "Jarāvasānaṁ yobbaññaṁ,",
  "thi-ap25:25.2_0": "rūpaṁ asucisammataṁ;",
  "thi-ap25:25.3_0": "Rogantamapicārogyaṁ,",
  "thi-ap25:25.4_0": "jīvitaṁ maraṇantikaṁ.",
  "thi-ap25:26.1_0": "Idampi te subhaṁ rūpaṁ,",
  "thi-ap25:26.2_0": "sasīkantaṁ manoharaṁ;",
  "thi-ap25:26.3_0": "Bhūsanānaṁ alaṅkāraṁ,",
  "thi-ap25:26.4_0": "sirisaṅghāṭasannibhaṁ.",
  "thi-ap25:27.1_0": "Puñjitaṁ lokasāraṁva,",
  "thi-ap25:27.2_0": "nayanānaṁ rasāyanaṁ;",
  "thi-ap25:27.3_0": "Puññānaṁ kittijananaṁ,",
  "thi-ap25:27.4_0": "ukkākakulanandanaṁ.",
  "thi-ap25:28.1_0": "Na cireneva kālena,",
  "thi-ap25:28.2_0": "jarā samadhisessati;",
  "thi-ap25:28.3_0": "Vihāya gehaṁ kāruññe,",
  "thi-ap25:28.4_0": "cara dhammamanindite’.",
  "thi-ap25:29.1_0": "Sutvāhaṁ mātu vacanaṁ,",
  "thi-ap25:29.2_0": "pabbajiṁ anagāriyaṁ;",
  "thi-ap25:29.3_0": "Dehena na tu cittena,",
  "thi-ap25:29.4_0": "rūpayobbanalāḷitā.",
  "thi-ap25:30.1_0": "Mahatā ca payattena,",
  "thi-ap25:30.2_0": "jhānajjhena paraṁ mama;",
  "thi-ap25:30.3_0": "Kātuñca vadate mātā,",
  "thi-ap25:30.4_0": "na cāhaṁ tattha ussukā.",
  "thi-ap25:31.1_0": "Tato mahākāruṇiko,",
  "thi-ap25:31.2_0": "disvā maṁ kāmalālasaṁ;",
  "thi-ap25:31.3_0": "Nibbindanatthaṁ rūpasmiṁ,",
  "thi-ap25:31.4_0": "mama cakkhupathe jino.",
  "thi-ap25:32.1_0": "Sakena ānubhāvena,",
  "thi-ap25:32.2_0": "itthiṁ māpesi sobhiniṁ;",
  "thi-ap25:32.3_0": "Dassanīyaṁ suruciraṁ,",
  "thi-ap25:32.4_0": "mamatopi surūpiniṁ.",
  "thi-ap25:33.1_0": "Tamahaṁ vimhitā disvā,",
  "thi-ap25:33.2_0": "ativimhitadehiniṁ;",
  "thi-ap25:33.3_0": "Cintayiṁ ‘saphalaṁ meti,",
  "thi-ap25:33.4_0": "nettalābhañca mānusaṁ.",
  "thi-ap25:34.1_0": "Tamahaṁ ehi subhage,",
  "thi-ap25:34.2_0": "yenattho taṁ vadehi me;",
  "thi-ap25:34.3_0": "Kulaṁ te nāmagottañca,",
  "thi-ap25:34.4_0": "vada me yadi te piyaṁ.",
  "thi-ap25:35.1_0": "Na vañcakālo subhage,",
  "thi-ap25:35.2_0": "ucchaṅge maṁ nivāsaya;",
  "thi-ap25:35.3_0": "Sīdantīva mamaṅgāni,",
  "thi-ap25:35.4_0": "pasuppaya muhuttakaṁ’.",
  "thi-ap25:36.1_0": "Tato sīsaṁ mamaṅge sā,",
  "thi-ap25:36.2_0": "katvā sayi sulocanā;",
  "thi-ap25:36.3_0": "Tassā nalāṭe patitā,",
  "thi-ap25:36.4_0": "luddhā paramadāruṇā.",
  "thi-ap25:37.1_0": "Saha tassā nipātena,",
  "thi-ap25:37.2_0": "piḷakā upapajjatha;",
  "thi-ap25:37.3_0": "Pagghariṁsu pabhinnā ca,",
  "thi-ap25:37.4_0": "kuṇapā pubbalohitā.",
  "thi-ap25:38.1_0": "Pabhinnaṁ vadanañcāpi,",
  "thi-ap25:38.2_0": "kuṇapaṁ pūtigandhanaṁ;",
  "thi-ap25:38.3_0": "Uddhumātaṁ vinilañca,",
  "thi-ap25:38.4_0": "pubbañcāpi sarīrakaṁ.",
  "thi-ap25:39.1_0": "Sā paveditasabbaṅgī,",
  "thi-ap25:39.2_0": "nissasantī muhuṁ muhuṁ;",
  "thi-ap25:39.3_0": "Vedayantī sakaṁ dukkhaṁ,",
  "thi-ap25:39.4_0": "karuṇaṁ paridevayi.",
  "thi-ap25:40.1_0": "‘Dukkhena dukkhitā homi,",
  "thi-ap25:40.2_0": "phusayanti ca vedanā;",
  "thi-ap25:40.3_0": "Mahādukkhe nimuggamhi,",
  "thi-ap25:40.4_0": "saraṇaṁ hohi me sakhī.",
  "thi-ap25:41.1_0": "Kuhiṁ vadanasobhaṁ te,",
  "thi-ap25:41.2_0": "kuhiṁ te tuṅganāsikā;",
  "thi-ap25:41.3_0": "Tambabimbavaroṭṭhaṁ te,",
  "thi-ap25:41.4_0": "vadanaṁ te kuhiṁ gataṁ.",
  "thi-ap25:42.1_0": "Kuhiṁ sasīnibhaṁ vaṇṇaṁ,",
  "thi-ap25:42.2_0": "kambugīvā kuhiṁ gatā;",
  "thi-ap25:42.3_0": "Doḷālolāva te kaṇṇā,",
  "thi-ap25:42.4_0": "vevaṇṇaṁ samupāgatā.",
  "thi-ap25:43.1_0": "Makuḷakhārakākārā,",
  "thi-ap25:43.2_0": "kalikāva payodharā;",
  "thi-ap25:43.3_0": "Pabhinnā pūtikuṇapā,",
  "thi-ap25:43.4_0": "duṭṭhagandhittamāgatā.",
  "thi-ap25:44.1_0": "Vedimajjhāva sussoṇī,",
  "thi-ap25:44.2_0": "sūnāva nītakibbisā;",
  "thi-ap25:44.3_0": "Jātā amejjhabharitā,",
  "thi-ap25:44.4_0": "aho rūpamasassataṁ.",
  "thi-ap25:45.1_0": "Sabbaṁ sarīrasañjātaṁ,",
  "thi-ap25:45.2_0": "pūtigandhaṁ bhayānakaṁ;",
  "thi-ap25:45.3_0": "Susānamiva bībhacchaṁ,",
  "thi-ap25:45.4_0": "ramante yattha bālisā’.",
  "thi-ap25:46.1_0": "Tadā mahākāruṇiko,",
  "thi-ap25:46.2_0": "bhātā me lokanāyako;",
  "thi-ap25:46.3_0": "Disvā saṁviggacittaṁ maṁ,",
  "thi-ap25:46.4_0": "imā gāthā abhāsatha.",
  "thi-ap25:47.1_0": "‘Āturaṁ kuṇapaṁ pūtiṁ,",
  "thi-ap25:47.2_0": "passa nande samussayaṁ;",
  "thi-ap25:47.3_0": "Asubhāya cittaṁ bhāvehi,",
  "thi-ap25:47.4_0": "ekaggaṁ susamāhitaṁ.",
  "thi-ap25:48.1_0": "Yathā idaṁ tathā etaṁ,",
  "thi-ap25:48.2_0": "yathā etaṁ tathā idaṁ;",
  "thi-ap25:48.3_0": "Duggandhaṁ pūtikaṁ vāti,",
  "thi-ap25:48.4_0": "bālānaṁ abhinanditaṁ.",
  "thi-ap25:49.1_0": "Evametaṁ avekkhantī,",
  "thi-ap25:49.2_0": "rattindivamatanditā;",
  "thi-ap25:49.3_0": "Tato sakāya paññāya,",
  "thi-ap25:49.4_0": "abhinibbijjha dakkhasi’.",
  "thi-ap25:50.1_0": "Tatohaṁ atisaṁviggā,",
  "thi-ap25:50.2_0": "sutvā gāthā subhāsitā;",
  "thi-ap25:50.3_0": "Tatraṭṭhitāvahaṁ santī,",
  "thi-ap25:50.4_0": "arahattamapāpuṇiṁ.",
  "thi-ap25:51.1_0": "Yattha yattha nisinnāhaṁ,",
  "thi-ap25:51.2_0": "sadā jhānaparāyaṇā;",
  "thi-ap25:51.3_0": "Jino tasmiṁ guṇe tuṭṭho,",
  "thi-ap25:51.4_0": "etadagge ṭhapesi maṁ.",
  "thi-ap25:52.1_0": "Kilesā jhāpitā mayhaṁ,",
  "thi-ap25:52.2_0": "…pe…",
  "thi-ap25:52.3_0": "viharāmi anāsavā.",
  "thi-ap25:53.1_0": "Svāgataṁ vata me āsi,",
  "thi-ap25:53.2_0": "…pe…",
  "thi-ap25:53.3_0": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap25:54.1_0": "Paṭisambhidā catasso,",
  "thi-ap25:54.2_0": "…pe…",
  "thi-ap25:54.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "thi-ap25:55.1_0": "Itthaṁ sudaṁ nandā bhikkhunī janapadakalyāṇī imā gāthāyo abhāsitthāti.",
  "thi-ap25:56.1_0": "Nandātheriyāpadānaṁ pañcamaṁ."
}