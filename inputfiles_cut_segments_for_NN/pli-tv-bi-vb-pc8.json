{
  "pli-tv-bi-vb-pc8:0.1_0": "Theravāda Vinaya",
  "pli-tv-bi-vb-pc8:0.2_0": "Bhikkhunivibhaṅga",
  "pli-tv-bi-vb-pc8:0.3_0": "Pācittiyakaṇḍa",
  "pli-tv-bi-vb-pc8:0.4_0": "Lasuṇavagga",
  "pli-tv-bi-vb-pc8:0.5_0": "8. Uccārachaḍḍanasikkhāpada",
  "pli-tv-bi-vb-pc8:1.1_0": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bi-vb-pc8:1.2_0": "Tena kho pana samayena aññataro brāhmaṇo nibbiṭṭharājabhaṭo “taññeva bhaṭapathaṁ yācissāmī”ti sīsaṁ nahāyitvā bhikkhunupassayaṁ nissāya rājakulaṁ gacchati.",
  "pli-tv-bi-vb-pc8:1.3_0": "Aññatarā bhikkhunī kaṭāhe vaccaṁ katvā tirokuṭṭe chaḍḍentī tassa brāhmaṇassa matthake āsumbhi.",
  "pli-tv-bi-vb-pc8:1.4_0": "Atha kho so brāhmaṇo ujjhāyati khiyyati vipāceti—",
  "pli-tv-bi-vb-pc8:1.5_0": "“assamaṇiyo imā muṇḍā bandhakiniyo.",
  "pli-tv-bi-vb-pc8:1.6_0": "Kathañhi nāma gūthakaṭāhaṁ matthake āsumbhissanti.",
  "pli-tv-bi-vb-pc8:1.7_0": "Imāsaṁ upassayaṁ jhāpessāmī”ti.",
  "pli-tv-bi-vb-pc8:1.8_0": "Ummukaṁ gahetvā upassayaṁ pavisati.",
  "pli-tv-bi-vb-pc8:1.9_0": "Aññataro upāsako upassayā nikkhamanto addasa taṁ brāhmaṇaṁ ummukaṁ gahetvā upassayaṁ pavisantaṁ.",
  "pli-tv-bi-vb-pc8:1.10_0": "Disvāna taṁ brāhmaṇaṁ etadavoca—",
  "pli-tv-bi-vb-pc8:1.11_0": "“kissa tvaṁ, bho,",
  "pli-tv-bi-vb-pc8:1.11_1": "ummukaṁ gahetvā upassayaṁ pavisasī”ti?",
  "pli-tv-bi-vb-pc8:1.12_0": "“Imā maṁ, bho,",
  "pli-tv-bi-vb-pc8:1.12_1": "muṇḍā bandhakiniyo gūthakaṭāhaṁ matthake āsumbhiṁsu.",
  "pli-tv-bi-vb-pc8:1.13_0": "Imāsaṁ upassayaṁ jhāpessāmī”ti.",
  "pli-tv-bi-vb-pc8:1.14_0": "“Gaccha, bho brāhmaṇa, maṅgalaṁ etaṁ.",
  "pli-tv-bi-vb-pc8:1.15_0": "Sahassaṁ lacchasi tañca bhaṭapathan”ti.",
  "pli-tv-bi-vb-pc8:1.16_0": "Atha kho so brāhmaṇo sīsaṁ nahāyitvā rājakulaṁ gantvā sahassaṁ alattha tañca bhaṭapathaṁ.",
  "pli-tv-bi-vb-pc8:1.17_0": "Atha kho so upāsako upassayaṁ pavisitvā bhikkhunīnaṁ etamatthaṁ ārocetvā paribhāsi.",
  "pli-tv-bi-vb-pc8:1.18_0": "Yā tā bhikkhuniyo appicchā …pe… tā ujjhāyanti khiyyanti vipācenti—",
  "pli-tv-bi-vb-pc8:1.19_0": "“kathañhi nāma bhikkhuniyo uccāraṁ tirokuṭṭe chaḍḍessantī”ti …pe…",
  "pli-tv-bi-vb-pc8:1.20_0": "“saccaṁ kira, bhikkhave,",
  "pli-tv-bi-vb-pc8:1.20_1": "bhikkhuniyo uccāraṁ tirokuṭṭe chaḍḍentī”ti?",
  "pli-tv-bi-vb-pc8:1.21_0": "“Saccaṁ, bhagavā”ti.",
  "pli-tv-bi-vb-pc8:1.22_0": "Vigarahi buddho bhagavā …pe…",
  "pli-tv-bi-vb-pc8:1.23_0": "kathañhi nāma, bhikkhave,",
  "pli-tv-bi-vb-pc8:1.23_1": "bhikkhuniyo uccāraṁ tirokuṭṭe chaḍḍessanti.",
  "pli-tv-bi-vb-pc8:1.24_0": "Netaṁ, bhikkhave, appasannānaṁ vā pasādāya …pe…",
  "pli-tv-bi-vb-pc8:1.25_0": "evañca pana, bhikkhave,",
  "pli-tv-bi-vb-pc8:1.25_1": "bhikkhuniyo imaṁ sikkhāpadaṁ uddisantu—",
  "pli-tv-bi-vb-pc8:1.26.1_0": "“Yā pana bhikkhunī uccāraṁ vā passāvaṁ vā saṅkāraṁ vā vighāsaṁ vā tirokuṭṭe vā tiropākāre vā chaḍḍeyya vā chaḍḍāpeyya vā, pācittiyan”ti.",
  "pli-tv-bi-vb-pc8:2.1.1_0": "Yā panāti",
  "pli-tv-bi-vb-pc8:2.1.2_0": "yā yādisā …pe…",
  "pli-tv-bi-vb-pc8:2.1.3_0": "bhikkhunīti",
  "pli-tv-bi-vb-pc8:2.1.4_0": "…pe… ayaṁ imasmiṁ atthe adhippetā bhikkhunīti.",
  "pli-tv-bi-vb-pc8:2.1.5_0": "Uccāro nāma",
  "pli-tv-bi-vb-pc8:2.1.6_0": "gūtho vuccati.",
  "pli-tv-bi-vb-pc8:2.1.7_0": "Passāvo nāma",
  "pli-tv-bi-vb-pc8:2.1.8_0": "muttaṁ vuccati.",
  "pli-tv-bi-vb-pc8:2.1.9_0": "Saṅkāraṁ nāma",
  "pli-tv-bi-vb-pc8:2.1.10_0": "kacavaraṁ vuccati.",
  "pli-tv-bi-vb-pc8:2.1.11_0": "Vighāsaṁ nāma",
  "pli-tv-bi-vb-pc8:2.1.12_0": "calakāni vā aṭṭhikāni vā ucchiṭṭhodakaṁ vā.",
  "pli-tv-bi-vb-pc8:2.1.13_0": "Kuṭṭo nāma",
  "pli-tv-bi-vb-pc8:2.1.14_0": "tayo kuṭṭā— iṭṭhakākuṭṭo, silākuṭṭo, dārukuṭṭo.",
  "pli-tv-bi-vb-pc8:2.1.15_0": "Pākāro nāma",
  "pli-tv-bi-vb-pc8:2.1.16_0": "tayo pākārā— iṭṭhakāpākāro, silāpākāro, dārupākāro.",
  "pli-tv-bi-vb-pc8:2.1.17_0": "Tirokuṭṭeti",
  "pli-tv-bi-vb-pc8:2.1.18_0": "kuṭṭassa parato.",
  "pli-tv-bi-vb-pc8:2.1.19_0": "Tiropākāreti",
  "pli-tv-bi-vb-pc8:2.1.20_0": "pākārassa parato.",
  "pli-tv-bi-vb-pc8:2.1.21_0": "Chaḍḍeyyāti",
  "pli-tv-bi-vb-pc8:2.1.22_0": "sayaṁ chaḍḍeti, āpatti pācittiyassa.",
  "pli-tv-bi-vb-pc8:2.1.23_0": "Chaḍḍāpeyyāti",
  "pli-tv-bi-vb-pc8:2.1.24_0": "aññaṁ āṇāpeti, āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc8:2.1.25_0": "Sakiṁ āṇattā bahukampi chaḍḍeti, āpatti pācittiyassa.",
  "pli-tv-bi-vb-pc8:2.2.1_0": "Anāpatti—",
  "pli-tv-bi-vb-pc8:2.2.2_0": "oloketvā chaḍḍeti,",
  "pli-tv-bi-vb-pc8:2.2.3_0": "avaḷañje chaḍḍeti,",
  "pli-tv-bi-vb-pc8:2.2.4_0": "ummattikāya,",
  "pli-tv-bi-vb-pc8:2.2.5_0": "ādikammikāyāti.",
  "pli-tv-bi-vb-pc8:2.2.6_0": "Aṭṭhamasikkhāpadaṁ niṭṭhitaṁ."
}