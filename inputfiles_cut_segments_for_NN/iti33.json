{
  "iti33:0.1_0": "Itivuttaka 33",
  "iti33:0.2_0": "Dukanipāta",
  "iti33:0.3_0": "Paṭhamavagga",
  "iti33:0.4_0": "Dutiyasīlasutta",
  "iti33:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti33:2.1_0": "“Dvīhi, bhikkhave,",
  "iti33:2.1_1": "dhammehi samannāgato puggalo yathābhataṁ nikkhitto evaṁ sagge.",
  "iti33:2.2_0": "Katamehi dvīhi?",
  "iti33:2.3_0": "Bhaddakena ca sīlena, bhaddikāya ca diṭṭhiyā.",
  "iti33:2.4_0": "Imehi kho, bhikkhave,",
  "iti33:2.4_1": "dvīhi dhammehi samannāgato puggalo yathābhataṁ nikkhitto evaṁ sagge”ti.",
  "iti33:2.5_0": "Etamatthaṁ bhagavā avoca.",
  "iti33:2.6_0": "Tatthetaṁ iti vuccati:",
  "iti33:3.1_0": "“Bhaddakena ca sīlena,",
  "iti33:3.2_0": "bhaddikāya ca diṭṭhiyā;",
  "iti33:3.3_0": "Etehi dvīhi dhammehi,",
  "iti33:3.4_0": "yo samannāgato naro;",
  "iti33:3.5_0": "Kāyassa bhedā sappañño,",
  "iti33:3.6_0": "saggaṁ so upapajjatī”ti.",
  "iti33:4.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti33:4.2_0": "Chaṭṭhaṁ."
}