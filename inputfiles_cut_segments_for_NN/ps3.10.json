{
  "ps3.10:0.1_0": "Paṭisambhidāmagga",
  "ps3.10:0.2_0": "3 Paññāvagga",
  "ps3.10:0.3_0": "3.10. Mātikākathā",
  "ps3.10:1.1_0": "Nicchāto, mokkho vimokkho, vijjāvimutti, adhisīlaṁ, adhicittaṁ, adhipaññā, passaddhi, ñāṇaṁ, dassanaṁ, visuddhi, nekkhammaṁ, nissaraṇaṁ, paviveko, vosaggo, cariyā, jhānavimokkho, bhāvanā, adhiṭṭhānaṁ, jīvitaṁ. [19]",
  "ps3.10:2.1_0": "**Nicchāto**ti nekkhammena kāmacchandato nicchāto,",
  "ps3.10:2.1_1": "abyāpādena byāpādato nicchāto …pe…",
  "ps3.10:2.2_0": "paṭhamena jhānena nīvaraṇehi nicchāto …pe…",
  "ps3.10:2.3_0": "arahattamaggena sabbakilesehi nicchāto. ",
  "ps3.10:3.1_0": "**Mokkho vimokkho**ti nekkhammena kāmacchandato muccatīti—",
  "ps3.10:3.2_0": "mokkho vimokkho.",
  "ps3.10:3.3_0": "Abyāpādena byāpādato muccatīti—",
  "ps3.10:3.4_0": "mokkho vimokkho …pe…",
  "ps3.10:3.5_0": "paṭhamena jhānena nīvaraṇehi muccatīti—",
  "ps3.10:3.6_0": "mokkho vimokkho …pe…",
  "ps3.10:3.7_0": "arahattamaggena sabbakilesehi muccatīti—",
  "ps3.10:3.8_0": "mokkho vimokkho. ",
  "ps3.10:4.1_0": "**Vijjāvimuttī**ti nekkhammaṁ vijjatīti vijjā,",
  "ps3.10:4.1_1": "kāmacchandato muccatīti vimutti.",
  "ps3.10:4.2_0": "Vijjanto muccati, muccanto vijjatīti—",
  "ps3.10:4.3_0": "vijjāvimutti.",
  "ps3.10:4.4_0": "Abyāpādo vijjatīti vijjā,",
  "ps3.10:4.4_1": "byāpādato vimuccatīti vimutti.",
  "ps3.10:4.5_0": "Vijjanto muccati, muccanto vijjatīti—",
  "ps3.10:4.6_0": "vijjāvimutti …pe…",
  "ps3.10:4.7_0": "arahattamaggo vijjatīti vijjā,",
  "ps3.10:4.7_1": "sabbakilesehi muccatīti vimutti.",
  "ps3.10:4.8_0": "Vijjanto muccati, muccanto vijjatīti—",
  "ps3.10:4.9_0": "vijjāvimutti. ",
  "ps3.10:5.1_0": "**Adhisīlaṁ adhicittaṁ adhipaññā**ti nekkhammena kāmacchandaṁ, saṁvaraṭṭhena sīlavisuddhi,",
  "ps3.10:5.1_1": "avikkhepaṭṭhena cittavisuddhi, dassanaṭṭhena diṭṭhivisuddhi.",
  "ps3.10:5.2_0": "Yo tattha saṁvaraṭṭho, ayaṁ adhisīlasikkhā.",
  "ps3.10:5.3_0": "Yo tattha avikkhepaṭṭho, ayaṁ adhicittasikkhā.",
  "ps3.10:5.4_0": "Yo tattha dassanaṭṭho, ayaṁ adhipaññāsikkhā.",
  "ps3.10:5.5_0": "Abyāpādena byāpādaṁ saṁvaraṭṭhena sīlavisuddhi …pe…",
  "ps3.10:5.6_0": "arahattamaggena sabbakilese saṁvaraṭṭhena sīlavisuddhi,",
  "ps3.10:5.6_1": "avikkhepaṭṭhena cittavisuddhi.",
  "ps3.10:5.7_0": "Dassanaṭṭhena diṭṭhivisuddhi.",
  "ps3.10:5.8_0": "Yo tattha saṁvaraṭṭho, ayaṁ adhisīlasikkhā.",
  "ps3.10:5.9_0": "Yo tattha avikkhepaṭṭho, ayaṁ adhicittasikkhā.",
  "ps3.10:5.10_0": "Yo tattha dassanaṭṭho, ayaṁ adhipaññāsikkhā.  ",
  "ps3.10:6.1_0": "**Passaddhī**ti nekkhammena kāmacchandaṁ paṭippassambheti,",
  "ps3.10:6.1_1": "abyāpādena byāpādaṁ paṭippassambheti …pe…",
  "ps3.10:6.2_0": "arahattamaggena sabbakilese paṭippassambheti. ",
  "ps3.10:7.1_0": "**Ñāṇan**ti kāmacchandassa pahīnattā nekkhammaṁ ñātaṭṭhena ñāṇaṁ;",
  "ps3.10:7.2_0": "byāpādassa pahīnattā abyāpādo ñātaṭṭhena ñāṇaṁ …pe…",
  "ps3.10:7.3_0": "sabbakilesānaṁ pahīnattā arahattamaggo ñātaṭṭhena ñāṇaṁ. ",
  "ps3.10:8.1_0": "**Dassanan**ti kāmacchandassa pahīnattā nekkhammaṁ diṭṭhattā dassanaṁ.",
  "ps3.10:8.2_0": "Byāpādassa pahīnattā abyāpādo diṭṭhattā dassanaṁ …pe…",
  "ps3.10:8.3_0": "sabbakilesānaṁ pahīnattā arahattamaggo diṭṭhattā dassanaṁ. ",
  "ps3.10:9.1_0": "**Visuddhī**ti kāmacchandaṁ pajahanto nekkhammena visujjhati.",
  "ps3.10:9.2_0": "Byāpādaṁ pajahanto abyāpādena visujjhati …pe…",
  "ps3.10:9.3_0": "sabbakilese pajahanto arahattamaggena visujjhati. ",
  "ps3.10:10.1_0": "**Nekkhamman**ti kāmānametaṁ nissaraṇaṁ, yadidaṁ nekkhammaṁ.",
  "ps3.10:10.2_0": "Rūpānametaṁ nissaraṇaṁ, yadidaṁ āruppaṁ.",
  "ps3.10:10.3_0": "Yaṁ kho pana kiñci bhūtaṁ saṅkhataṁ paṭiccasamuppannaṁ, nirodho tassa nekkhammaṁ.",
  "ps3.10:10.4_0": "Byāpādassa abyāpādo nekkhammaṁ.",
  "ps3.10:10.5_0": "Thinamiddhassa ālokasaññā nekkhammaṁ …pe…",
  "ps3.10:10.6_0": "sabbakilesānaṁ arahattamaggo nekkhammaṁ. ",
  "ps3.10:11.1_0": "**Nissaraṇan**ti kāmānametaṁ nissaraṇaṁ, yadidaṁ nekkhammaṁ.",
  "ps3.10:11.2_0": "Rūpānametaṁ nissaraṇaṁ, yadidaṁ āruppaṁ.",
  "ps3.10:11.3_0": "Yaṁ kho pana kiñci bhūtaṁ saṅkhataṁ paṭiccasamuppannaṁ, nirodho tassa nissaraṇaṁ.",
  "ps3.10:11.4_0": "Kāmacchandassa nekkhammaṁ nissaraṇaṁ.",
  "ps3.10:11.5_0": "Byāpādassa abyāpādo nissaraṇaṁ …pe…",
  "ps3.10:11.6_0": "sabbakilesānaṁ arahattamaggo nissaraṇaṁ. ",
  "ps3.10:12.1_0": "**Paviveko**ti kāmacchandassa nekkhammaṁ paviveko …pe…",
  "ps3.10:12.2_0": "sabbakilesānaṁ arahattamaggo paviveko. ",
  "ps3.10:13.1_0": "**Vosaggo**ti nekkhammena kāmacchandaṁ vosajjatīti—",
  "ps3.10:13.2_0": "vosaggo.",
  "ps3.10:13.3_0": "Abyāpādena byāpādaṁ vosajjatīti—",
  "ps3.10:13.4_0": "vosaggo …pe…",
  "ps3.10:13.5_0": "arahattamaggena sabbakilese vosajjatīti—",
  "ps3.10:13.6_0": "vosaggo. ",
  "ps3.10:14.1_0": "**Cariyā**ti kāmacchandaṁ pajahanto nekkhammena carati.",
  "ps3.10:14.2_0": "Byāpādaṁ pajahanto abyāpādena carati …pe…",
  "ps3.10:14.3_0": "sabbakilese pajahanto arahattamaggena carati. ",
  "ps3.10:15.1_0": "**Jhānavimokkho**ti nekkhammaṁ jhāyatīti—",
  "ps3.10:15.2_0": "jhānaṁ.",
  "ps3.10:15.3_0": "Kāmacchandaṁ jhāpetīti—",
  "ps3.10:15.4_0": "jhānaṁ.",
  "ps3.10:15.5_0": "Jhāyanto muccatīti—",
  "ps3.10:15.6_0": "jhānavimokkho.",
  "ps3.10:15.7_0": "Jhāpento muccatīti—",
  "ps3.10:15.8_0": "jhānavimokkho.",
  "ps3.10:15.9_0": "Jhāyantīti dhammā.",
  "ps3.10:15.10_0": "Jhāpentīti kilese.",
  "ps3.10:15.11_0": "Jhāte ca jhāpe ca jānātīti—",
  "ps3.10:15.12_0": "jhānajhāyī.",
  "ps3.10:15.13_0": "Abyāpādo jhāyatīti jhānaṁ.",
  "ps3.10:15.14_0": "Byāpādaṁ jhāpetīti—",
  "ps3.10:15.15_0": "jhānaṁ …pe…",
  "ps3.10:15.16_0": "ālokasaññā jhāyatīti—",
  "ps3.10:15.17_0": "jhānaṁ.",
  "ps3.10:15.18_0": "Thinamiddhaṁ jhāpetīti—",
  "ps3.10:15.19_0": "jhānaṁ …pe…",
  "ps3.10:15.20_0": "arahattamaggo jhāyatīti—",
  "ps3.10:15.21_0": "jhānaṁ.",
  "ps3.10:15.22_0": "Sabbakilese jhāpetīti—",
  "ps3.10:15.23_0": "jhānaṁ.",
  "ps3.10:15.24_0": "Jhāyanto muccatīti—",
  "ps3.10:15.25_0": "jhānavimokkho.",
  "ps3.10:15.26_0": "Jhāpento muccatīti—",
  "ps3.10:15.27_0": "jhānavimokkho.",
  "ps3.10:15.28_0": "Jhāyantīti—",
  "ps3.10:15.29_0": "dhammā.",
  "ps3.10:15.30_0": "Jhāpentīti—",
  "ps3.10:15.31_0": "kilese.",
  "ps3.10:15.32_0": "Jhāte ca jhāpe ca jānātīti—",
  "ps3.10:15.33_0": "jhānajhāyī. ",
  "ps3.10:16.1_0": "**Bhāvanā adhiṭṭhānaṁ jīvitan**ti kāmacchandaṁ pajahanto nekkhammaṁ bhāvetīti—",
  "ps3.10:16.2_0": "bhāvanāsampanno.",
  "ps3.10:16.3_0": "Nekkhammavasena cittaṁ adhiṭṭhātīti—",
  "ps3.10:16.4_0": "adhiṭṭhānasampanno.",
  "ps3.10:16.5_0": "Svāyaṁ evaṁ bhāvanāsampanno adhiṭṭhānasampanno samaṁ jīvati, no visamaṁ;",
  "ps3.10:16.6_0": "sammā jīvati, no micchā;",
  "ps3.10:16.7_0": "visuddhaṁ jīvati, no kiliṭṭhanti—",
  "ps3.10:16.8_0": "ājīvasampanno.",
  "ps3.10:16.9_0": "Svāyaṁ evaṁ bhāvanāsampanno adhiṭṭhānasampanno ājīvasampanno yaññadeva parisaṁ upasaṅkamati—",
  "ps3.10:16.10_0": "yadi khattiyaparisaṁ yadi brāhmaṇaparisaṁ yadi gahapatiparisaṁ yadi samaṇaparisaṁ—",
  "ps3.10:16.11_0": "visārado upasaṅkamati amaṅkubhūto.",
  "ps3.10:16.12_0": "Taṁ kissa hetu?",
  "ps3.10:16.13_0": "Tathā hi so bhāvanāsampanno adhiṭṭhānasampanno ājīvasampanno.",
  "ps3.10:17.1_0": "Byāpādaṁ pajahanto abyāpādaṁ bhāvetīti—",
  "ps3.10:17.2_0": "bhāvanāsampanno …pe…",
  "ps3.10:17.3_0": "thinamiddhaṁ pajahanto ālokasaññaṁ bhāvetīti—",
  "ps3.10:17.4_0": "bhāvanāsampanno …pe…",
  "ps3.10:17.5_0": "uddhaccaṁ pajahanto avikkhepaṁ bhāvetīti—",
  "ps3.10:17.6_0": "bhāvanāsampanno …pe…",
  "ps3.10:17.7_0": "vicikicchaṁ pajahanto dhammavavatthānaṁ bhāvetīti—",
  "ps3.10:17.8_0": "bhāvanāsampanno …pe…",
  "ps3.10:17.9_0": "avijjaṁ pajahanto vijjaṁ bhāvetīti—",
  "ps3.10:17.10_0": "bhāvanāsampanno …pe…",
  "ps3.10:17.11_0": "aratiṁ pajahanto pāmojjaṁ bhāvetīti—",
  "ps3.10:17.12_0": "bhāvanāsampanno …pe…",
  "ps3.10:17.13_0": "nīvaraṇe pajahanto paṭhamaṁ jhānaṁ bhāvetīti—",
  "ps3.10:17.14_0": "bhāvanāsampanno …pe…",
  "ps3.10:17.15_0": "sabbakilese pajahanto arahattamaggaṁ bhāvetīti—",
  "ps3.10:17.16_0": "bhāvanāsampanno.",
  "ps3.10:17.17_0": "Arahattamaggavasena cittaṁ adhiṭṭhātīti—",
  "ps3.10:17.18_0": "adhiṭṭhānasampanno.",
  "ps3.10:17.19_0": "Svāyaṁ evaṁ bhāvanāsampanno adhiṭṭhānasampanno samaṁ jīvati, no visamaṁ;",
  "ps3.10:17.20_0": "sammā jīvati, no micchā;",
  "ps3.10:17.21_0": "visuddhaṁ jīvati, no kiliṭṭhanti—",
  "ps3.10:17.22_0": "ājīvasampanno.",
  "ps3.10:17.23_0": "Svāyaṁ evaṁ bhāvanāsampanno adhiṭṭhānasampanno ājīvasampanno yaññadeva parisaṁ upasaṅkamati—",
  "ps3.10:17.24_0": "yadi khattiyaparisaṁ yadi brāhmaṇaparisaṁ yadi gahapatiparisaṁ yadi samaṇaparisaṁ—",
  "ps3.10:17.25_0": "visārado upasaṅkamati amaṅkubhūto.",
  "ps3.10:17.26_0": "Taṁ kissa hetu?",
  "ps3.10:17.27_0": "Tathā hi so bhāvanāsampanno adhiṭṭhānasampanno ājīvasampannoti.  ",
  "ps3.10:17.28_0": "Mātikākathā niṭṭhitā.",
  "ps3.10:17.29_0": "Paññāvaggo tatiyo.",
  "ps3.10:17.30_0": "Tassuddānaṁ",
  "ps3.10:18.1_0": "Paññā iddhi abhisamayo,",
  "ps3.10:18.2_0": "viveko cariyapañcamo;",
  "ps3.10:18.3_0": "Pāṭihāri samasīsi,",
  "ps3.10:18.4_0": "satipaṭṭhānā vipassanā;",
  "ps3.10:18.5_0": "Tatiye paññāvaggamhi,",
  "ps3.10:18.6_0": "mātikāya ca te dasāti.",
  "ps3.10:19.1_0": "Mahāvaggo yuganaddho,",
  "ps3.10:19.2_0": "paññāvaggo ca nāmato;",
  "ps3.10:19.3_0": "Tayova vaggā imamhi,",
  "ps3.10:19.4_0": "paṭisambhidāpakaraṇe.",
  "ps3.10:20.1_0": "Anantanayamaggesu,",
  "ps3.10:20.2_0": "gambhīro sāgarūpamo;",
  "ps3.10:20.3_0": "Nabhañca tārakākiṇṇaṁ,",
  "ps3.10:20.4_0": "thūlo jātassaro yathā;",
  "ps3.10:20.5_0": "Kathikānaṁ visālāya,",
  "ps3.10:20.6_0": "yogīnaṁ ñāṇajotananti.",
  "ps3.10:20.7_0": "Paṭisambhidāmaggapāḷi niṭṭhitā."
}