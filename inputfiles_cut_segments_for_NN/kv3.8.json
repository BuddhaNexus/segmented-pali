{
  "kv3.8:0.1_0": "Kathāvatthu",
  "kv3.8:0.2_0": "Mahāpaṇṇāsaka",
  "kv3.8:0.3_0": "Tatiyavagga",
  "kv3.8:0.4_0": "Dibbasotakathā",
  "kv3.8:1.1_0": "Maṁsasotaṁ dhammupatthaddhaṁ dibbasotaṁ hotīti?",
  "kv3.8:1.2_0": "Āmantā.",
  "kv3.8:1.3_0": "Maṁsasotaṁ dibbasotaṁ, dibbasotaṁ maṁsasotanti?",
  "kv3.8:1.4_0": "Na hevaṁ vattabbe …pe….",
  "kv3.8:2.1_0": "Maṁsasotaṁ dhammupatthaddhaṁ dibbasotaṁ hotīti?",
  "kv3.8:2.2_0": "Āmantā.",
  "kv3.8:2.3_0": "Yādisaṁ maṁsasotaṁ tādisaṁ dibbasotaṁ,",
  "kv3.8:2.3_1": "yādisaṁ dibbasotaṁ tādisaṁ maṁsasotanti?",
  "kv3.8:2.4_0": "Na hevaṁ vattabbe …pe….",
  "kv3.8:3.1_0": "Maṁsasotaṁ dhammupatthaddhaṁ dibbasotaṁ hotīti?",
  "kv3.8:3.2_0": "Āmantā.",
  "kv3.8:3.3_0": "Taññeva maṁsasotaṁ taṁ dibbasotaṁ,",
  "kv3.8:3.3_1": "taṁ dibbasotaṁ taṁ maṁsasotanti?",
  "kv3.8:3.4_0": "Na hevaṁ vattabbe …pe….",
  "kv3.8:4.1_0": "Maṁsasotaṁ dhammupatthaddhaṁ dibbasotaṁ hotīti?",
  "kv3.8:4.2_0": "Āmantā.",
  "kv3.8:4.3_0": "Yādiso maṁsasotassa visayo ānubhāvo gocaro tādiso dibbassa sotassa visayo ānubhāvo gocaroti?",
  "kv3.8:4.4_0": "Na hevaṁ vattabbe …pe….",
  "kv3.8:5.1_0": "Maṁsasotaṁ dhammupatthaddhaṁ dibbasotaṁ hotīti?",
  "kv3.8:5.2_0": "Āmantā.",
  "kv3.8:5.3_0": "Upādinnaṁ hutvā anupādinnaṁ hotīti?",
  "kv3.8:5.4_0": "Na hevaṁ vattabbe …pe….",
  "kv3.8:6.1_0": "Upādinnaṁ hutvā anupādinnaṁ hotīti?",
  "kv3.8:6.2_0": "Āmantā.",
  "kv3.8:6.3_0": "Kāmāvacaraṁ hutvā rūpāvacaraṁ hotīti?",
  "kv3.8:6.4_0": "Na hevaṁ vattabbe …pe….",
  "kv3.8:7.1_0": "Kāmāvacaraṁ hutvā rūpāvacaraṁ hotīti?",
  "kv3.8:7.2_0": "Āmantā.",
  "kv3.8:7.3_0": "Rūpāvacaraṁ hutvā arūpāvacaraṁ hotīti?",
  "kv3.8:7.4_0": "Na hevaṁ vattabbe …pe….",
  "kv3.8:8.1_0": "Rūpāvacaraṁ hutvā arūpāvacaraṁ hotīti?",
  "kv3.8:8.2_0": "Āmantā.",
  "kv3.8:8.3_0": "Pariyāpannaṁ hutvā apariyāpannaṁ hotīti?",
  "kv3.8:8.4_0": "Na hevaṁ vattabbe …pe….",
  "kv3.8:9.1_0": "Maṁsasotaṁ dhammupatthaddhaṁ dibbasotaṁ hotīti?",
  "kv3.8:9.2_0": "Āmantā.",
  "kv3.8:9.3_0": "Dibbasotaṁ dhammupatthaddhaṁ maṁsasotaṁ hotīti?",
  "kv3.8:9.4_0": "Na hevaṁ vattabbe …pe….",
  "kv3.8:10.1_0": "Maṁsasotaṁ dhammupatthaddhaṁ dibbasotaṁ hotīti?",
  "kv3.8:10.2_0": "Āmantā.",
  "kv3.8:10.3_0": "Ekaṁyeva sotanti?",
  "kv3.8:10.4_0": "Na hevaṁ vattabbe …pe….",
  "kv3.8:11.1_0": "Ekaṁyeva sotanti?",
  "kv3.8:11.2_0": "Āmantā.",
  "kv3.8:11.3_0": "Nanu dve sotāni vuttāni bhagavatā—",
  "kv3.8:11.4_0": "“maṁsasotaṁ, dibbasotan”ti?",
  "kv3.8:11.5_0": "Āmantā.",
  "kv3.8:11.6_0": "Hañci dve sotāni vuttāni bhagavatā—",
  "kv3.8:11.7_0": "maṁsasotaṁ, dibbasotaṁ, no ca vata re vattabbe—",
  "kv3.8:11.8_0": "“ekaññeva sotan”ti.",
  "kv3.8:11.9_0": "Dibbasotakathā niṭṭhitā."
}