{
  "kv18.9:0.1_0": "Kathāvatthu",
  "kv18.9:0.2_0": "Mahāpaṇṇāsaka",
  "kv18.9:0.3_0": "Aṭṭhārasamavagga",
  "kv18.9:0.4_0": "Cakkhunārūpaṁpassatītikathā",
  "kv18.9:1.1_0": "Cakkhunā rūpaṁ passatīti?",
  "kv18.9:1.2_0": "Āmantā.",
  "kv18.9:1.3_0": "Rūpena rūpaṁ passatīti?",
  "kv18.9:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv18.9:1.5_0": "rūpena rūpaṁ passatīti?",
  "kv18.9:1.6_0": "Āmantā.",
  "kv18.9:1.7_0": "Rūpena rūpaṁ paṭivijānātīti?",
  "kv18.9:1.8_0": "Na hevaṁ vattabbe …pe…",
  "kv18.9:1.9_0": "rūpena rūpaṁ paṭivijānātīti?",
  "kv18.9:1.10_0": "Āmantā.",
  "kv18.9:1.11_0": "Rūpaṁ manoviññāṇanti?",
  "kv18.9:1.12_0": "Na hevaṁ vattabbe …pe…",
  "kv18.9:1.13_0": "cakkhunā rūpaṁ passatīti?",
  "kv18.9:1.14_0": "Āmantā.",
  "kv18.9:1.15_0": "Atthi cakkhussa āvaṭṭanā …pe… paṇidhīti?",
  "kv18.9:1.16_0": "Na hevaṁ vattabbe …pe…",
  "kv18.9:1.17_0": "nanu natthi cakkhussa āvaṭṭanā …pe… paṇidhīti?",
  "kv18.9:1.18_0": "Āmantā.",
  "kv18.9:1.19_0": "Hañci natthi cakkhussa āvaṭṭanā …pe… paṇidhi, no ca vata re vattabbe—",
  "kv18.9:1.20_0": "“cakkhunā rūpaṁ passatī”ti.",
  "kv18.9:2.1_0": "Sotena saddaṁ suṇātīti …pe…",
  "kv18.9:2.2_0": "ghānena gandhaṁ ghāyatīti …pe…",
  "kv18.9:2.3_0": "jivhāya rasaṁ sāyatīti …pe…",
  "kv18.9:2.4_0": "kāyena phoṭṭhabbaṁ phusatīti?",
  "kv18.9:2.5_0": "Āmantā.",
  "kv18.9:2.6_0": "Rūpena rūpaṁ phusatīti?",
  "kv18.9:2.7_0": "Na hevaṁ vattabbe …pe….",
  "kv18.9:3.1_0": "Rūpena rūpaṁ phusatīti?",
  "kv18.9:3.2_0": "Āmantā.",
  "kv18.9:3.3_0": "Rūpena rūpaṁ paṭivijānātīti?",
  "kv18.9:3.4_0": "Na hevaṁ vattabbe …pe…",
  "kv18.9:3.5_0": "rūpena rūpaṁ paṭivijānātīti?",
  "kv18.9:3.6_0": "Āmantā.",
  "kv18.9:3.7_0": "Rūpaṁ manoviññāṇanti?",
  "kv18.9:3.8_0": "Na hevaṁ vattabbe …pe…",
  "kv18.9:3.9_0": "kāyena phoṭṭhabbaṁ phusatīti?",
  "kv18.9:3.10_0": "Āmantā.",
  "kv18.9:3.11_0": "Atthi kāyassa āvaṭṭanā …pe… paṇidhīti?",
  "kv18.9:3.12_0": "Na hevaṁ vattabbe …pe…",
  "kv18.9:3.13_0": "nanu natthi kāyassa āvaṭṭanā …pe… paṇidhīti?",
  "kv18.9:3.14_0": "Āmantā.",
  "kv18.9:3.15_0": "Hañci natthi kāyassa āvaṭṭanā …pe… paṇidhi, no ca vata re vattabbe—",
  "kv18.9:3.16_0": "“kāyena phoṭṭhabbaṁ phusatī”ti …pe….",
  "kv18.9:4.1_0": "Na vattabbaṁ—",
  "kv18.9:4.2_0": "“cakkhunā rūpaṁ passatī”ti …pe…",
  "kv18.9:4.3_0": "“kāyena phoṭṭhabbaṁ phusatī”ti?",
  "kv18.9:4.4_0": "Āmantā.",
  "kv18.9:4.5_0": "Nanu vuttaṁ bhagavatā—",
  "kv18.9:4.6_0": "“idha, bhikkhave,",
  "kv18.9:4.6_1": "bhikkhu cakkhunā rūpaṁ passati …pe…",
  "kv18.9:4.7_0": "kāyena phoṭṭhabbaṁ phusatī”ti.",
  "kv18.9:4.8_0": "Attheva suttantoti?",
  "kv18.9:4.9_0": "Āmantā.",
  "kv18.9:4.10_0": "Tena hi cakkhunā rūpaṁ passati …pe…",
  "kv18.9:4.11_0": "kāyena phoṭṭhabbaṁ phusatīti.",
  "kv18.9:4.12_0": "Cakkhunā rūpaṁ passatītikathā niṭṭhitā.",
  "kv18.9:4.13_0": "Aṭṭhārasamo vaggo.",
  "kv18.9:4.14_0": "Tassuddānaṁ",
  "kv18.9:5.1_0": "Buddho bhagavā manussaloke aṭṭhāsi,",
  "kv18.9:5.2_0": "buddhena bhagavatā dhammo desito,",
  "kv18.9:5.3_0": "natthi buddhassa bhagavato karuṇā,",
  "kv18.9:5.4_0": "buddhassa bhagavato uccārapassāvo ativiya aññe gandhajāte adhiggaṇhāti,",
  "kv18.9:5.5_0": "ekena ariyamaggena cattāri sāmaññaphalāni sacchikaroti,",
  "kv18.9:5.6_0": "jhānā jhānaṁ saṅkamati,",
  "kv18.9:5.7_0": "atthi jhānantarikā,",
  "kv18.9:5.8_0": "samāpanno saddaṁ suṇāti,",
  "kv18.9:5.9_0": "cakkhunā rūpaṁ passati …pe…",
  "kv18.9:5.10_0": "kāyena phoṭṭhabbaṁ phusati."
}