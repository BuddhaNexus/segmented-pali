{
  "anya-e0401n6:0_0": "7. Samuccayasaṅgahaanudīpanā",
  "anya-e0401n6:1_0": "162.",
  "anya-e0401n6:1_1": "Samuccayasaṅgahe .",
  "anya-e0401n6:1_2": "Attano āveṇikabhūtena sāmañña lakkhaṇenati ca sambandho.",
  "anya-e0401n6:1_3": "Aññāpadesena eva tadubhayalakkhaṇena salakkhaṇāni nāma vuccantīti adhippāyo.",
  "anya-e0401n6:1_4": "‘‘Nibbānassapi sarūpato labbhamānasabhāvatā’’ti aññanissaya rahitena labbhamānasabhāvatā.",
  "anya-e0401n6:1_5": "Nanu nibbānampi rāgakkhayo dosakkhayo mohakkhayotiādinā aññanissayadassanaṁ atthi yevāti.",
  "anya-e0401n6:1_6": "Dassanamattaṁ atthi.",
  "anya-e0401n6:1_7": "Nibbānaṁ pana rāgādīhi paṭibaddhaṁ na hoti.",
  "anya-e0401n6:1_8": "Atha kho tehi dūrataraṁ hoti.",
  "anya-e0401n6:1_9": "Paṭipakkhataraṁ,",
  "anya-e0401n6:1_10": "paṭiviruddhataraṁ.",
  "anya-e0401n6:1_11": "Yañhi rūpassa khayo vayo bhedo aniccāti vuttaṁ.",
  "anya-e0401n6:1_12": "Tattha rūpassa uppajjitvā khayo vuttoti so rūpassa nissito eva hoti.",
  "anya-e0401n6:1_13": "Idha pana rāgādīnaṁ puna uppādassapi abhāvo vuttoti so rāgādīsu anissito eva.",
  "anya-e0401n6:1_14": "Na kevalaṁ anissito.",
  "anya-e0401n6:1_15": "Atha kho tehi dūrataro ca paṭipakkhataro ca tesaṁ paṭipakkhaguṇehi imassasiddhattāti.",
  "anya-e0401n6:1_16": "Anipphannarūpānipi adhippetāni eva tesampi khandhāyatanadhātu saccesu saṅgahitattā.",
  "anya-e0401n6:2_0": "Āsavādīsu.",
  "anya-e0401n6:2_1": "‘‘Pārivāsiyaṭṭhenā’’ti parivāsakaraṇaṭṭhena.",
  "anya-e0401n6:2_2": "‘‘Madanīyaṭṭhenā’’ti madanajanakaṭṭhena.",
  "anya-e0401n6:2_3": "‘‘Parivāsaṁ gaṇhantī’’ti dosavepullaṁ āpajjantīti vuttaṁ hoti.",
  "anya-e0401n6:2_4": "Puna ‘‘parivāsaṁ’’ti duggandhatādiparivāsaṁ.",
  "anya-e0401n6:2_5": "‘‘Āsavabharitamevā’’ti āsavehi pūritameva.",
  "anya-e0401n6:2_6": "‘‘Chaḷārammaṇāni dūsentī’’ti tāni sāsavādibhāvaṁ pāpentīti adhippāyo.",
  "anya-e0401n6:2_7": "‘‘Bhavato’’ti bhūmito iccevattho.",
  "anya-e0401n6:2_8": "Avadhīyati paricchindīyati etasmāti avadhi.",
  "anya-e0401n6:2_9": "Apādānaṁ vuccati.",
  "anya-e0401n6:2_10": "Mariyādo nāma pariyantaparicchedo.",
  "anya-e0401n6:2_11": "Mariyādamattabhūto kriyāvisayo mariyādavisayo.",
  "anya-e0401n6:2_12": "Avadhi nāma byāpana vidhānaṁ,",
  "anya-e0401n6:2_13": "kriyā byāpanassa visayo abhividhivisayo.",
  "anya-e0401n6:2_14": "‘‘Yassā’’ti avadhi vatthussa.",
  "anya-e0401n6:2_15": "‘‘Attānaṁ’’ti avadhivatthuṁ.",
  "anya-e0401n6:2_16": "‘‘Bahikatvā’’ti sampattamattaṁ katvāti adhippāyo.",
  "anya-e0401n6:2_17": "Avadhivicāraṇāyaṁ.",
  "anya-e0401n6:2_18": "‘‘Saddassā’’ti bhagavato kittisaddassa.",
  "anya-e0401n6:2_19": "‘‘Taṁ’’ti avadhibhūtaṁ atthaṁ.",
  "anya-e0401n6:2_20": "‘‘Yaso’’ti kittisaddo.",
  "anya-e0401n6:2_21": "‘‘Itaraṁ’’ti anabhividhivisayaṁ bahi katvā pavattati.",
  "anya-e0401n6:3_0": "Kāmāsavādīsu.",
  "anya-e0401n6:3_1": "‘‘Tannāmenā’’ti kāmanāmena.",
  "anya-e0401n6:3_2": "‘‘Tadārammaṇā’’ti kāmadhammārammaṇā.",
  "anya-e0401n6:3_3": "‘‘Ayamattho vā’’ti kāmīyatītiādinā vutto pacchimatthova.",
  "anya-e0401n6:3_4": "‘‘Mahaggatakusaladhammā’’ti idhādhippetaṁ kammabhavaṁ sandhāya vuttaṁ.",
  "anya-e0401n6:3_5": "‘‘Taṁ nibbattā’’ti tena nibbattā.",
  "anya-e0401n6:3_6": "‘‘Tadārammaṇā’’ti duvidhabhavārammaṇā.",
  "anya-e0401n6:3_7": "‘‘Taṇhā evā’’ti bhavataṇhā eva.",
  "anya-e0401n6:3_8": "‘‘Bhavo evā’’ti bhavārammaṇatāya bhavanāmikā taṇhā eva.",
  "anya-e0401n6:3_9": "‘‘Ime evā’’ti taṇhādiṭṭhi avijjā eva.",
  "anya-e0401n6:3_10": "‘‘Parivutthe satī’’ti parivasite sati.",
  "anya-e0401n6:3_11": "‘‘Kāmavisayā’’ti kāmadhammavisayā.",
  "anya-e0401n6:3_12": "‘‘Tasmiṁ’’ti kāmāsave.",
  "anya-e0401n6:3_13": "Bhavavisayā mānādayo parivutthā evātiādinā yojetabbaṁ.",
  "anya-e0401n6:3_14": "Tathā diṭṭhivisayāti padepi.",
  "anya-e0401n6:4_0": "‘‘Anassāsikaṁ katvā’’ti assāsapassāsarahitaṁ katvā.",
  "anya-e0401n6:4_1": "‘‘Avahananaṭṭhenā’’ti ajjhottharitvā māraṇaṭṭhena.",
  "anya-e0401n6:4_2": "Adhokatvā māraṇaṭṭhenātipi yujjati.",
  "anya-e0401n6:4_3": "‘‘Duttaraṭṭhenā’’ti tattha patantassa tarituṁ dukkaraṭṭhena.",
  "anya-e0401n6:4_4": "‘‘Vuttanayenā’’ti āsavesu vuttanayena.",
  "anya-e0401n6:5_0": "‘‘Vaṭṭasmiṁ’’ti tividhavaṭṭasmiṁ.",
  "anya-e0401n6:5_1": "‘‘Bhavayantake’’ti avijjāsaṅkhārādike bhavacakke.",
  "anya-e0401n6:5_2": "‘‘Āmasanaṁ’’ti padassa atthaṁ dasseti ‘‘tathā tathā kappetvā gahaṇa’’nti.",
  "anya-e0401n6:5_3": "‘‘Sāsane’’ti pariyattisāsane tasmiṁ tasmiṁ suttante.",
  "anya-e0401n6:5_4": "‘‘Diṭṭhiyo diṭṭhupādānaṁ’’ti diṭṭhivatthūsu daḷhaggāhaṭṭhena diṭṭhiyo eva diṭṭhupādānaṁ.",
  "anya-e0401n6:5_5": "Attavādupādāne.",
  "anya-e0401n6:5_6": "Parikappa buddhi nāma micchāñāṇaṁ vuccati.",
  "anya-e0401n6:5_7": "‘‘Issara nimmitaṁ’’ti sakalalokissarena mahābrahmunā ādikappakāle nimmitaṁ.",
  "anya-e0401n6:5_8": "‘‘Adhicca samuppannaṁ’’ti ahetu apaccayā samuppannaṁ.",
  "anya-e0401n6:5_9": "‘‘Accantasassataṁ’’ti bhavaparamparāsu sassataṁ.",
  "anya-e0401n6:5_10": "‘‘Ekaccasassataṁ’’ti bhavavisesaṁ patvā ekaccānaṁ sattānaṁ sassataṁ.",
  "anya-e0401n6:5_11": "‘‘Ucchinnaṁ’’ti yatthakatthaci parammaraṇā ucchinnaṁ.",
  "anya-e0401n6:5_12": "Purāṇañcakammaṁ parikkhīṇaṁ,",
  "anya-e0401n6:5_13": "navañcakammaṁ akataṁ.",
  "anya-e0401n6:5_14": "Evaṁ saṁsāra suddhītiādinā gahaṇanti attho.",
  "anya-e0401n6:5_15": "Santo kāyo sakkāyo.",
  "anya-e0401n6:5_16": "‘‘Santo’’ti paramatthato vijjamāno.",
  "anya-e0401n6:5_17": "‘‘Kāyo’’ti rūpakāyo, nāmakāyo.",
  "anya-e0401n6:5_18": "Attano attano kāyo vā sakkāyo.",
  "anya-e0401n6:5_19": "Paccattakāyo,",
  "anya-e0401n6:5_20": "pāṭipuggalika kāyoti vuttaṁ hoti.",
  "anya-e0401n6:5_21": "Yathāvuttakāya dvayameva.",
  "anya-e0401n6:5_22": "Sakkāye diṭṭhi sakkāyadiṭṭhi.",
  "anya-e0401n6:5_23": "Tattha ‘‘sakkāye diṭṭhī’’ti pubbantāparanta kappikānaṁ viya pubbantā paranteacintetvā sabbasattānaṁpi",
  "anya-e0401n6:5_24": "attano khandhesu eva ‘rūpaṁ me attāti vā’ attā me rūpavāti vā,",
  "anya-e0401n6:5_25": "attani me rūpanti vā,",
  "anya-e0401n6:5_26": "rūpasmiṁ me attāti vā,",
  "anya-e0401n6:5_27": "evamādinā dhammatā siddhā diṭṭhīti vuttaṁ hoti.",
  "anya-e0401n6:5_28": "‘‘Assutavā’’ti khandhadesanādike suññatadhammappaṭisaṁ yutte desanā dhamme assuta pubbattā natthi sutaṁ etassāti assutavā.",
  "anya-e0401n6:5_29": "‘‘Puthujjano’’ti lokiyamahājano.",
  "anya-e0401n6:5_30": "Tattha pariyāpanno pana ekapuggalopi puthujjanotveva vuccati.",
  "anya-e0401n6:5_31": "So sutavāpi atthi,",
  "anya-e0401n6:5_32": "assutavāpi atthi.",
  "anya-e0401n6:5_33": "Idha assutavā adhippeto.",
  "anya-e0401n6:5_34": "Ariya puggalo pana tattha pariyāpanno na hoti. Ahantivā,",
  "anya-e0401n6:5_35": "mamāti vā,",
  "anya-e0401n6:5_36": "mayīti vā, meti vā,",
  "anya-e0401n6:5_37": "parāmasana padāni nāma.",
  "anya-e0401n6:5_38": "‘‘Sesadhammevā gahetvā’’ti rūpato avasese nāmakkhandha dhamme attā meti gahetvā vā.",
  "anya-e0401n6:5_39": "‘‘Dhamma muttakaṁ vā attānaṁ gahetvā’’ti pañcakkhandhadhammavimuttaṁ parikappasiddhaṁ attānaṁ vā gahetvā.",
  "anya-e0401n6:5_40": "Catasso avatthā yassāti caturāvatthikā.",
  "anya-e0401n6:5_41": "Vedanāya sambhogarasattā ‘‘saṁbhuñjiṁ’’ti vuttaṁ.",
  "anya-e0401n6:5_42": "‘‘Sukhito’’ti sukhavedanāya samaṅgīpuggalo.",
  "anya-e0401n6:5_43": "Dhammato khandha pañcakameva.",
  "anya-e0401n6:5_44": "Tattha pana sukhavedanāpadhānattā tathā samanupassanto vedanaṁ attāti samanupassati nāma.",
  "anya-e0401n6:5_45": "‘‘Samūhato gahetvā’’ti ahamasmi,",
  "anya-e0401n6:5_46": "ahaṁ eko sattotiādinā samūhato.",
  "anya-e0401n6:5_47": "‘‘Vatthū’’ti pañcakkhandhā vuccanti.",
  "anya-e0401n6:5_48": "Ñātapariññādivasena apariññātāni vatthūni etehīti apariññātavatthukā.",
  "anya-e0401n6:5_49": "Ekamuhuttamattepi kāle.",
  "anya-e0401n6:5_50": "Rūpaṁ attato samanupassatītiādikaṁ caturāvatthaṁ sandhāya ‘‘kadāci attato’’tiādi vuttaṁ.",
  "anya-e0401n6:5_51": "‘‘Attanimittaṁ’’ti abhikkamanādīsu kāyavacīmano kriyāsu ahaṁ abhikkamāmi,",
  "anya-e0401n6:5_52": "ahaṁ paṭikkamāmītiādinā citte dissamānā attacchāyā vuccati.",
  "anya-e0401n6:6_0": "‘‘Kāmanaṭṭhenā’’ti icchanaṭṭhena.",
  "anya-e0401n6:6_1": "‘‘Chandanaṭṭhenā’’ti patthanaṭṭhena.",
  "anya-e0401n6:6_2": "Līnabhāvo nāma cittacetasikānaṁ paṭikuṭanaṁ.",
  "anya-e0401n6:6_3": "Āpādīyate āpādanaṁ.",
  "anya-e0401n6:6_4": "Līnabhāvassa āpādananti viggaho.",
  "anya-e0401n6:6_5": "‘‘Tandī’’ti ālasyaṁ vuccati.",
  "anya-e0401n6:6_6": "‘‘Vijambhitatā’’ nāma kilesavasena kāyaṅgānaṁ vijambhanaṁ samiñjanappasāraṇādikaraṇaṁ.",
  "anya-e0401n6:6_7": "Sā eva paccayo etassāti viggaho.",
  "anya-e0401n6:7_0": "Anusayapadatthe.",
  "anya-e0401n6:7_1": "‘‘Uppajjantī’’ti uppajjituṁ sakkonti.",
  "anya-e0401n6:7_2": "Na pana ekantato uppajjanti.",
  "anya-e0401n6:7_3": "Santesuhi ekantato uppajjantesu anusayā nāma na honti sayanakiccasseva abhāvato.",
  "anya-e0401n6:7_4": "‘‘Uppajjantī’’ti vā uppajjituṁ pahonti.",
  "anya-e0401n6:7_5": "Patthodano bahūnaṁ janānaṁ pahotītiādīsu viya.",
  "anya-e0401n6:7_6": "Paññattiyohi asabhāvadhammajātikattā kāraṇa lābhepi uppajjituṁ nappahonti.",
  "anya-e0401n6:7_7": "Ime pana sabhāvadhammajātikattā kāraṇa lābhe sati uppajjituṁ pahontīti.",
  "anya-e0401n6:7_8": "Evañhisati,",
  "anya-e0401n6:7_9": "uppādaṁ apattānaṁpi tesaṁ paramatthajātikatā siddhā hotīti.",
  "anya-e0401n6:7_10": "‘‘Saha anusentī’’ti ekato anusentīti vuttā kāmarāgānusayo ca paṭighānusayo ca mānānusayo ca diṭṭhānusayo ca vicikicchānusayo ca.",
  "anya-e0401n6:7_11": "Imesaṁ sattānaṁ sattasantāne anusayakiccamattaṁ ṭhapetvā ekato uppatti nāma natthi.",
  "anya-e0401n6:7_12": "Yadi ekato uppajjeyyuṁ.",
  "anya-e0401n6:7_13": "Dvādasā kusalacittāni sattasantāne niccakālampi ekato uppajjeyyuṁ.",
  "anya-e0401n6:7_14": "Na ca uppajjanti.",
  "anya-e0401n6:7_15": "Tasmā viññāyati uppajjanaṁ nāma appahīnaṭṭhena uppajjanārahabhāvo vuttoti.",
  "anya-e0401n6:7_16": "Sentīti vatvā tadatthaṁ dasseti ‘‘visuṁ’’tiādinā.",
  "anya-e0401n6:7_17": "‘‘Avuṭṭhitā’’ti uppādaṁ apattā.",
  "anya-e0401n6:7_18": "‘‘Tathāpavattā’’ti cālanākārena pavattā.",
  "anya-e0401n6:7_19": "Puna ‘‘tathāpavattā’’ti javanasahajātākārena pavattā.",
  "anya-e0401n6:7_20": "‘‘Yesaṁ’’ti kāmarāgānusayādīnaṁ.",
  "anya-e0401n6:7_21": "‘‘Āvajjanaṁ’’ti āvajjanacittaṁ.",
  "anya-e0401n6:7_22": "‘‘Damathaṁ’’ti sudantabhāvaṁ.",
  "anya-e0401n6:7_23": "‘‘Tathā pavattā’’ti cittasantānānusayanākārena pavattā.",
  "anya-e0401n6:7_24": "Tāavatthā yesaṁ te tadavatthikā.",
  "anya-e0401n6:7_25": "Yadi te uppādaṁ apattā. Evaṁsati,",
  "anya-e0401n6:7_26": "te paramatthāpi nāma na bhaveyyunti codanaṁ pariharati ‘‘te panā’’tiādinā.",
  "anya-e0401n6:7_27": "Sace te kusalābyākata cittasantānampi anugatā. Evaṁsati,",
  "anya-e0401n6:7_28": "te kusalābyākatāni nāma siyunti codanaṁ pariharati ‘‘na cā’’tiādinā.",
  "anya-e0401n6:7_29": "Atha te ekanta akusalā siyuṁ. Evaṁsati,",
  "anya-e0401n6:7_30": "kusalābyākatehi viruddhā bhaveyyuṁti codanaṁ pariharati ‘‘nāpī’’tiādinā.",
  "anya-e0401n6:7_31": "Yadi uppādaṁ apattā. Evaṁsati,",
  "anya-e0401n6:7_32": "kālavimuttā siyunti āha ‘‘nāpikālattaya vinimuttā’’tiādiṁ.",
  "anya-e0401n6:7_33": "‘‘Sānusaye cittasantāne’’ti sekkhaputhujjanānaṁ cittasantāne.",
  "anya-e0401n6:7_34": "‘‘Saha magguppādā’’ti magguppādena saheva.",
  "anya-e0401n6:7_35": "‘‘Tattha tattha vutto’’ti aṭṭhakathāṭīkāsu vutto.",
  "anya-e0401n6:7_36": "‘‘Anāgatasāmaññaṁ’’ti anāgatasadisaṁ.",
  "anya-e0401n6:7_37": "Na ekanta anāgatantipi vadanti.",
  "anya-e0401n6:7_38": "Kathaṁ te saṅkhatajātikā hontīti āha ‘‘tehi magge’’tiādiṁ.",
  "anya-e0401n6:7_39": "Vibhāvanipāṭhe.",
  "anya-e0401n6:7_40": "‘‘Appahīnā’’ti maggena appahīnā.",
  "anya-e0401n6:7_41": "‘‘Tadavatthā’’ti uppajjanārahāvatthā.",
  "anya-e0401n6:7_42": "‘‘Taṁ sabhāvattā’’ti kāmarāgādi sabhāvattā.",
  "anya-e0401n6:7_43": "‘‘Tathā vuccantī’’ti anusayāti vuccanti.",
  "anya-e0401n6:7_44": "Anāgatā nāma na honti.",
  "anya-e0401n6:7_45": "Cittasantāne vattamānabhāvena siddhattā.",
  "anya-e0401n6:7_46": "‘‘Hañci pajahatī’’ti yadipajahati.",
  "anya-e0401n6:7_47": "‘‘Tenahī’’ti tato eva.",
  "anya-e0401n6:7_48": "‘‘Ratto’’ti rāgasamaṅgī hutvā.",
  "anya-e0401n6:7_49": "‘‘Duṭṭho’’ti dosasamaṅgī hutvā.",
  "anya-e0401n6:7_50": "‘‘Muḷho’’ti mohasamaṅgī hutvā pajahatīti doso āpajjatīti vuttaṁ hoti.",
  "anya-e0401n6:7_51": "Pariyuṭṭhāna pattānaṁ rāgādīnaṁ.",
  "anya-e0401n6:7_52": "‘‘Maggavajjhaṁ’’ti maggena vadhitabbaṁ.",
  "anya-e0401n6:7_53": "‘‘Uppannaṁ’’ti paccuppannaṁ.",
  "anya-e0401n6:7_54": "Vattamānañca taṁ uppannañcāti vattamānuppannaṁ.",
  "anya-e0401n6:7_55": "‘‘Bhutvā’’ti ārammaṇaṁ paribhuñjitvā.",
  "anya-e0401n6:7_56": "Vigacchatīti vigataṁ.",
  "anya-e0401n6:7_57": "Bhutvā vigatañca taṁ uppannañcāti bhutvā vigatuppannaṁ.",
  "anya-e0401n6:7_58": "Vipaccanatthāya okāsaṁ karontīti okāsakataṁ.",
  "anya-e0401n6:7_59": "Okāsakatañca taṁ uppannañcāti okāsakatuppannaṁ.",
  "anya-e0401n6:7_60": "Samudācāro vuccati bhiyyo pavattanaṁ.",
  "anya-e0401n6:7_61": "Samudācāro ca so uppannañcāti samudācāruppannaṁ.",
  "anya-e0401n6:7_62": "Khandhapañcaka saṅkhātaṁ bhūmiṁ labhatīti bhūmiladdhaṁ.",
  "anya-e0401n6:7_63": "Bhūmiladdhañca taṁ uppannañcāti bhūmiladdhuppannaṁ.",
  "anya-e0401n6:7_64": "Ārammaṇaṁ adhikataraṁ gaṇhātīti ārammaṇādhiggahitaṁ.",
  "anya-e0401n6:7_65": "Ārammaṇādhiggahitañca taṁ uppannañcāti ārammaṇādhiggahituppannaṁ.",
  "anya-e0401n6:7_66": "Mahaggatajjhānena avikkhambhitañca taṁ uppannañcāti avikkhambhituppannaṁ.",
  "anya-e0401n6:7_67": "Maggena asamugghāṭitañca taṁ uppannañcāti asamugghāṭituppannaṁ.",
  "anya-e0401n6:7_68": "Evaṁ maggavajjhānaṁ anusayānaṁ uppannabhāvena vuttattā paccuppannatā pariyāyova tesaṁ vattabboti.",
  "anya-e0401n6:7_69": "‘‘Sekkhā’’ti sattasekkhapuggalā.",
  "anya-e0401n6:8_0": "Orambhāgo vuccati kāmalokoceva puthujjanabhāvo ca.",
  "anya-e0401n6:8_1": "Orambhāge sandissantīti orambhāgiyāni.",
  "anya-e0401n6:8_2": "Uddhaṁbhāgo vuccati mahaggatabhāvoceva ariyabhāvo ca.",
  "anya-e0401n6:8_3": "Uddhaṁbhāge sandissantīti uddhaṁbhāgiyāni. Tattha.",
  "anya-e0401n6:8_4": "Kāmacchando,",
  "anya-e0401n6:8_5": "byāpādo,ti imāni dvesaṁ yojanāni kāmalokasaṅkhāte orambhāge eva sandissanti. Diṭṭhi,",
  "anya-e0401n6:8_6": "vicikicchā,",
  "anya-e0401n6:8_7": "sīlabbataparāmāso,ti imāni tīṇi puthujjanabhāva saṅkhāte.",
  "anya-e0401n6:8_8": "Sesāni pana pañca mahaggatabhāvasaṅkhāte ca ariyabhāvasaṅkhāteca uddhaṁbhāgepi sandissanti. Athavā.",
  "anya-e0401n6:8_9": "Purimāni pañcayassa tāni maggena appahīnāni,",
  "anya-e0401n6:8_10": "taṁ uparibhavagge ṭhitaṁpi kāmalokasaṅkhātaṁ orambhāgaṁ ākaḍḍhanti,",
  "anya-e0401n6:8_11": "tasmā orambhāgāya saṁvattantīti orambhāgiyāni.",
  "anya-e0401n6:8_12": "Pacchimāni pañca yassa tāni appahīnāni,",
  "anya-e0401n6:8_13": "taṁ kāmaloke ṭhitaṁpi uddhaṁbhāgaṁ ākaḍḍhanti,",
  "anya-e0401n6:8_14": "tasmā uddhaṁbhāgāya saṁvattantīti uddhaṁbhāgiyāni.",
  "anya-e0401n6:8_15": "Tatthahi dverūpārūparāgā ekantena mahaggatabhāvaṁ ākaḍḍhantiyeva.",
  "anya-e0401n6:8_16": "Māno ca uddhaccañca avijjācāti imāni ca rūpārūparāgasahagatāni hutvā ākaḍḍhanti.",
  "anya-e0401n6:8_17": "Oraṁ heṭṭhimaṁ kāmalokaṁ bhajantīti orambhāgiyāni.",
  "anya-e0401n6:8_18": "Uddhaṁ rūpārūpalokaṁ bhajantīti uddhaṁbhāgiyānītipi vaṇṇenti.",
  "anya-e0401n6:8_19": "‘‘Itarāni panā’’ti dve issā saṁyojana macchariyasaṁyojanāni.",
  "anya-e0401n6:8_20": "‘‘Kamopana dvinnaṁ pī’’ti idha saṅgahe dvinnaṁpi anukkamopana.",
  "anya-e0401n6:9_0": "‘‘Vibādhentī’’ti vihiṁsanti. ‘‘Upatāpenticā’’ti upagantvā santāpenti. Sesamettha suviññeyyaṁ.",
  "anya-e0401n6:10_0": "Akusalasaṅgahānudīpanā niṭṭhitā.",
  "anya-e0401n6:11_0": "163.",
  "anya-e0401n6:11_1": "Missakasaṅgahe.",
  "anya-e0401n6:11_2": "‘‘Cittappaṭipādanaṁ cittaniyojanaṁ.",
  "anya-e0401n6:11_3": "‘‘Sugati duggati vivaṭṭasaṅkhātāsu cā’’ti sugatibhava duggati bhavanibbānasaṅkhātāsu ca.",
  "anya-e0401n6:11_4": "Nibbānañhi vaṭṭato gigatattā vivaṭṭanti vuccati.",
  "anya-e0401n6:11_5": "‘‘Dassanādīhi evā’’ti dassanasaṅkappanādīhi eva.",
  "anya-e0401n6:11_6": "Ujugati nāma hitasukhasaṁvattanikā pavatti vuccati.",
  "anya-e0401n6:11_7": "Vaṅkagatināma ahita dukkhasaṁvattanikā pavatti.",
  "anya-e0401n6:11_8": "‘‘Pathaṅgānī’’ti pathassamaggassa aṅgāni.",
  "anya-e0401n6:11_9": "Maggoti ca upāyo vuccatīti āha ‘‘upāyaṅgānī’’ti.",
  "anya-e0401n6:11_10": "‘‘Itarānī’’ti sammādiṭṭhi sammāsaṅkappādīni aṅgāni.",
  "anya-e0401n6:11_11": "Ujugatiyā gamanassa upāyaṅgāni.",
  "anya-e0401n6:11_12": "Imepi cattāro maggaṅga dhammā dhammā nāma āgatā.",
  "anya-e0401n6:11_13": "‘‘Tathā tathā pavattānaṁ’’ti musāvādādivasena pavattānaṁ.",
  "anya-e0401n6:11_14": "‘‘Nā nāmaggovā’’ti micchāmaggo ca sammāmaggo ca.",
  "anya-e0401n6:12_0": "‘‘Attādhīnavuttike’’ti attāyattappavattike.",
  "anya-e0401n6:12_1": "Attano gati nāma cakkhundriyādīnaṁ dassanādi kiccameva vuccati.",
  "anya-e0401n6:12_2": "‘‘Issarā adhipatino’’ti idaṁ ādhippaccatthapākaṭatthaṁ vuttaṁ.",
  "anya-e0401n6:12_3": "Attho pana bhāvappadhānavasena gahetabbo.",
  "anya-e0401n6:12_4": "Indriyaṁ issariyaṁ ādhippaccantihi ime ekatthāti.",
  "anya-e0401n6:12_5": "‘‘Itthākārāni’’ nāma itthiliṅgapurisaliṅgādi.",
  "anya-e0401n6:12_6": "‘‘Aññathā appavattiyaṁ’’ti itthisaṇṭhāne purisaliṅgādīnaṁ,",
  "anya-e0401n6:12_7": "purisasaṇṭhāne itthiliṅgādīnaṁ appavattiyaṁ.",
  "anya-e0401n6:12_8": "Tenāha ‘‘tathāhī’’tiādiṁ.",
  "anya-e0401n6:12_9": "Mano vijānanakicce sampayuttadhammānaṁ issaro hoti anaññābhibhavanīyabhāvena pavattanatoti sambandho.",
  "anya-e0401n6:12_10": "Evaṁ parattha.",
  "anya-e0401n6:12_11": "‘‘Ārammaṇādhimuccane’’ti ārammaṇe nirāsaṅkappavattiyaṁ.",
  "anya-e0401n6:12_12": "‘‘Ārammaṇupaṭṭhāne’’ti citte buddhaguṇādhikassa ārammaṇassa upaṭṭhāne.",
  "anya-e0401n6:12_13": "‘‘Catusaccadhammo’’ti tena puggalena anaññāta pubbo catusacca dhammo.",
  "anya-e0401n6:12_14": "Anaññāta pubbaṁ amataṁ vā padaṁ.",
  "anya-e0401n6:12_15": "Anaññātaṁ ñassāmi iti pavattaṁ indriyanti samāso.",
  "anya-e0401n6:12_16": "Paṭipannassāti pana atthato siddhattā vuttaṁ.",
  "anya-e0401n6:12_17": "‘‘Vippakatabhāvenā’’ti aniṭṭhaṅgatabhāvena.",
  "anya-e0401n6:12_18": "Punappunaṁ jānanakiccayuttānaṁ majjhe channaṁsekkhānaṁ.",
  "anya-e0401n6:12_19": "Etena avasaddassayāva sabbakilesappahānā jānananti atthaṁ dīpeti.",
  "anya-e0401n6:12_20": "Pathama maggena ñātaṁ mariyādaṁ anatikkamma jānanantipi vaṇṇenti.",
  "anya-e0401n6:12_21": "Ājānitatthāti aññātāvī.",
  "anya-e0401n6:12_22": "Arahā khīṇāsavo katakicco vusita brahmacariyo.",
  "anya-e0401n6:12_23": "Aññātāvino indriyanti aññātāvindriyanti atthaṁ dassetuṁ ‘‘pariniṭṭhita ājānanakiccassā’’tiādi vuttaṁ.",
  "anya-e0401n6:12_24": "‘‘Tabbimuttī’’ti attaggāha vimutti.",
  "anya-e0401n6:12_25": "‘‘Tasmiṁ vā’’ti tasmiṁ attanivā.",
  "anya-e0401n6:12_26": "‘‘Saṁkiliṭṭho’’ti nānākilesehi saṁkilesito.",
  "anya-e0401n6:12_27": "‘‘Vipphandito’’ti nānāsukhadukkhehi saṁkampito.",
  "anya-e0401n6:12_28": "‘‘Vodānapattiyā’’ti visuddhipattatthāya.",
  "anya-e0401n6:12_29": "‘‘Tāyapaṭi pattiyā’’ti karaṇatthe,",
  "anya-e0401n6:12_30": "hetu atthe vā karaṇavacanaṁ.",
  "anya-e0401n6:12_31": "‘‘Vodānapattiyā’’ti sāmiatthe sāmivacanaṁ.",
  "anya-e0401n6:13_0": "‘‘Balīyantī’’ti nāmadhātuniddeso.",
  "anya-e0401n6:13_1": "Tenāha ‘‘balasā karontī’’ti.",
  "anya-e0401n6:13_2": "‘‘Balasā’’ti balena.",
  "anya-e0401n6:13_3": "Parito samantato senti parivārentīti parissayā.",
  "anya-e0401n6:13_4": "Assaddhassa bhāvo assaddhiyaṁ.",
  "anya-e0401n6:13_5": "Kosajjasaṅkhātena paṭipakkhadhammena.",
  "anya-e0401n6:13_6": "Muṭṭhā naṭṭhā sati yassāti muṭṭhassati.",
  "anya-e0401n6:13_7": "Muṭṭhassatissa bhāvo muṭṭhassaccaṁ.",
  "anya-e0401n6:13_8": "‘‘Sesaduka dvayaṁ’’ti ahirikadukaṁ hiridukañca.",
  "anya-e0401n6:14_0": "Gahapati,",
  "anya-e0401n6:14_1": "jāyampatī,",
  "anya-e0401n6:14_2": "tiādīsu viya patisaddo sāmiissara pariyāyoti āha ‘‘patīti sāmiissaro’’ti.",
  "anya-e0401n6:14_3": "‘‘Paresaṁ’’ti sahajātesu paresaṁ indriyānaṁ.",
  "anya-e0401n6:14_4": "‘‘Parehī’’ti kammatthe karaṇavacanaṁ.",
  "anya-e0401n6:14_5": "Pare sahajāte indriya dhammeti attho.",
  "anya-e0401n6:14_6": "‘‘Adhiko’’ti adhikataro.",
  "anya-e0401n6:14_7": "‘‘Attādhīna vuttīnaṁ’’ti attāyattavuttīnaṁ.",
  "anya-e0401n6:14_8": "‘‘Chandāgamanavasena vā’’ti ettha āgacchati phalaṁ etenāti āgamanaṁ.",
  "anya-e0401n6:14_9": "Ādikāraṇaṁ,",
  "anya-e0401n6:14_10": "mūlakāraṇaṁ,",
  "anya-e0401n6:14_11": "pabhavakāraṇaṁ.",
  "anya-e0401n6:14_12": "Chando eva āgamanaṁ chandāgamanaṁ.",
  "anya-e0401n6:14_13": "Tenāha ‘‘pubbe atītabhavesū’’tiādiṁ.",
  "anya-e0401n6:14_14": "Sampayutta dhamme attaparādhīne katvāti sambandho.",
  "anya-e0401n6:14_15": "Mahogho tiṇapaṇṇakasaṭe attaparādhīne karonto viyāti yojanā.",
  "anya-e0401n6:14_16": "Tattha ‘‘attaparādhīne’’ti attasaṅkhātoparo attaparo.",
  "anya-e0401n6:14_17": "Attaparena adhīnā attaparādhīnāti attho.",
  "anya-e0401n6:14_18": "‘‘Sesesupī’’ti sesesu vīriya citta vīmaṁsādhipatīsupi.",
  "anya-e0401n6:14_19": "Etena ‘vīriyameva adhipati vīriyādhipati.",
  "anya-e0401n6:14_20": "Vīriyavato kiṁ nāma kammaṁ na sijjhatīti evaṁ pubbābhisaṅkhāravasena vā pubbe atītabhavesu suṭṭhu āsevitavīriyāgamanavasena",
  "anya-e0401n6:14_21": "vā tesu tesu kalyāṇa pāpakammesu sampayutta dhamme mahogho viya tiṇapaṇṇakasaṭe attaparādhīne niccaṁ",
  "anya-e0401n6:14_22": "paggahitadhurekatvā pavattaṁ vīriya’ntiādinayaṁ atidisati.",
  "anya-e0401n6:14_23": "‘‘Pacchimā tayo dhammā’’ti vīriya citta vīmaṁsā dhammā.",
  "anya-e0401n6:14_24": "Paravasepi vattanti,",
  "anya-e0401n6:14_25": "tadā tesaṁ adhipatibhāvo natthīti codanā.",
  "anya-e0401n6:14_26": "Adhipati kiccaṁ nāma dhuravāhitāti sambandho.",
  "anya-e0401n6:14_27": "‘‘Taṁ taṁ kicca visesaṁ anapekkhitvā’’ti cakkhundriyaṁ dassana kicce issaro,",
  "anya-e0401n6:14_28": "sotindriyaṁ savana kiccetiādīsu taṁ taṁ kiccavisesaṁ apekkhati,",
  "anya-e0401n6:14_29": "tathā anapekkhitvā.",
  "anya-e0401n6:14_30": "‘‘Vijānanādi kiccaṁ’’ti ‘mano ārammaṇa vijānane’tiādinā vuttaṁ sabbaṁ indriya kiccaṁ.",
  "anya-e0401n6:15_0": "Bhuso haranti vahantīti āhārā.",
  "anya-e0401n6:15_1": "‘‘Bhuso’’ti atirekataranti attho.",
  "anya-e0401n6:15_2": "Kathaṁ pana bhuso harantīti āha ‘‘sahajātādī’’tiādiṁ.",
  "anya-e0401n6:15_3": "Tete paccaya dhammā tete paccayuppanna dhammā cāti yojanā.",
  "anya-e0401n6:15_4": "‘‘Etthā’’ti imissaṁ pāḷiyaṁ.",
  "anya-e0401n6:15_5": "‘‘Paccayāhāro vā’’ti ettha hetu ārammaṇādiko sabbopaccayo paccayāhāro nāma.",
  "anya-e0401n6:15_6": "Āhāra paccayopana visuṁ eko paccayaviseso.",
  "anya-e0401n6:15_7": "‘‘Paccayaṭṭhenā’’ti hetu ārammaṇādi paccayaṭṭhena.",
  "anya-e0401n6:15_8": "‘‘Āhārabhūtā’’ti upatthambhanakiccavisesabhūtāti vuttaṁ hoti.",
  "anya-e0401n6:15_9": "‘‘Kaḷīraṅkurānaṁ’’ti kammatthe sāmivacanaṁ.",
  "anya-e0401n6:15_10": "‘‘Vaḍḍhiyā’’ti vaḍḍhanatthāya.",
  "anya-e0401n6:15_11": "‘‘Ṭhitiyā’’ti ṭhitatthāya.",
  "anya-e0401n6:15_12": "Imesattā vaṭṭe pavattantīti sambandho.",
  "anya-e0401n6:15_13": "Sesamettha suviññeyyaṁ.",
  "anya-e0401n6:15_14": "‘‘Pakkhe viyūhitvā’’ti pakkhe sañcāletvāti vuttaṁ hoti.",
  "anya-e0401n6:15_15": "‘‘Pakkhe’’ti vā pakkhehi vātaṁ byūhitvā.",
  "anya-e0401n6:15_16": "‘‘Tadanusayitassā’’ti tasmiṁ vipāka santāne anusayi tassa.",
  "anya-e0401n6:15_17": "‘‘Vibhāvetvā’’ti paccakkhato vibhūtaṁ pākaṭaṁ katvā.",
  "anya-e0401n6:15_18": "‘‘Alabhitvā’’ti cakkhumhi alabhitvā.",
  "anya-e0401n6:15_19": "Apassitvāti vuttaṁ hoti.",
  "anya-e0401n6:15_20": "‘‘Tatthevā’’ti nivāsaṭṭhāne eva.",
  "anya-e0401n6:15_21": "‘‘Sampayuttā’’ti viññāṇena sampayuttā.",
  "anya-e0401n6:15_22": "Bhaviṁsu vaḍḍhiṁ sūti bhūtā.",
  "anya-e0401n6:15_23": "Sambhavaṁ vuḍḍhikāraṇaṁ esanti gavesantīti sambhavesino.",
  "anya-e0401n6:15_24": "‘‘Vuttanayenā’’ti ‘yathā yavabījānaṁ vā sālibījānaṁ vā’tiādinā vuttanayena.",
  "anya-e0401n6:15_25": "‘‘Āhāretī’’ti pavatteti.",
  "anya-e0401n6:15_26": "‘‘Manosañcetanāhāro’’ti kusalākusala kammabhūto manosañcetanāhāro.",
  "anya-e0401n6:15_27": "‘‘Tayo bhave’’ti tayo kāmabhavādike upapattibhave.",
  "anya-e0401n6:15_28": "Ṭhitiyāti ca anuggahāyāti ca vuttaṁ.",
  "anya-e0401n6:15_29": "Itarathā uppattiyāti ca vuttaṁ siyāti adhippāyo.",
  "anya-e0401n6:16_0": "‘‘Dubbalakiccaṭṭhānavatthukattā’’ti dubbalakiccattā dubbalaṭṭhānattā dubbalavatthukattā.",
  "anya-e0401n6:16_1": "‘‘Abhinipāta mattāni hontī’’ti dassana savanādi mattāni hontīti adhippāyo.",
  "anya-e0401n6:16_2": "‘‘Vitakka pacchimakaṁ jhānaṁ’’ti jhānāni nāma vitakkassa pacchānugataṁ hotīti attho.",
  "anya-e0401n6:16_3": "‘‘Tatthā’’ti tesupañcaviññāṇesu.",
  "anya-e0401n6:16_4": "‘‘Tā’’ti vedanā ekaggatā.",
  "anya-e0401n6:16_5": "‘‘Taṁ’’ti jhānakiccaṁ.",
  "anya-e0401n6:16_6": "Ekaggatā balavatī na hotīti sambandho.",
  "anya-e0401n6:16_7": "‘‘Maggindriya balabhāvāyā’’ti maggindriya balabhāvatthāya.",
  "anya-e0401n6:16_8": "Lokuttaravipākāni javanakiccāni ca sādhipatikāni ca hontīti vuttaṁ ‘‘tibhūmakānī’’tiādi.",
  "anya-e0401n6:16_9": "Sesamettha subodhaṁ.",
  "anya-e0401n6:17_0": "Missakasaṅgahānudīpanā niṭṭhitā.",
  "anya-e0401n6:18_0": "164.",
  "anya-e0401n6:18_1": "Bodhipakkhiyasaṅgahe.",
  "anya-e0401n6:18_2": "‘‘Satipaṭṭhānā’’ti ettha pasaddo pamukhattho padhānatthoti āha ‘‘pamukhā padhānā hutvā’’ti.",
  "anya-e0401n6:18_3": "Ṭhāsaddo gatinivatti atthoti āha ‘‘cittagamanaṁ nivattetvā pavattantī’’ti.",
  "anya-e0401n6:18_4": "‘‘Kāye anupassanā’’ti kāye pavattā anupassanā.",
  "anya-e0401n6:18_5": "Punappunaṁ passanti yogino etāyāti anupassanā.",
  "anya-e0401n6:18_6": "Katamaṁ passantīti āha ‘‘assāsapassāsādikassā’’tiādiṁ.",
  "anya-e0401n6:18_7": "Kathañcataṁ passantīti vuttaṁ ‘‘taṁ taṁ kāyabhāvenā’’tiādi.",
  "anya-e0401n6:18_8": "Idāni punappuna saddassa atthaṁ dassento ‘‘yāvā’’tiādimāha.",
  "anya-e0401n6:18_9": "Vedanānu passanādīsupi esevanayo.",
  "anya-e0401n6:18_10": "Taṁ na sundaraṁ. Kasmā,",
  "anya-e0401n6:18_11": "sakalassa rūpakāyassa adhippetattā.",
  "anya-e0401n6:18_12": "Sesamettha suviññeyyaṁ.",
  "anya-e0401n6:18_13": "Yadi evantiādīsu.",
  "anya-e0401n6:18_14": "‘‘Itarāpi siddhā’’ti kāyānupassanādayopi siddhā. Kasmā,",
  "anya-e0401n6:18_15": "dhammānupassanāya antogadhattā.",
  "anya-e0401n6:18_16": "‘‘Tathā anupassantassā’’ti rūppanalakkhaṇato anupassantassa.",
  "anya-e0401n6:18_17": "‘‘Taṁ taṁ samūhabhūtarūpadhammavasenā’’ti assāsapassāsādi samūhabhūtassa rūpa dhammassa vasena.",
  "anya-e0401n6:18_18": "‘‘Subhādivipallāsassā’’ti subha sukha niccavipallāsassa.",
  "anya-e0401n6:18_19": "‘‘Oḷārikassā’’ti supākaṭavasena oḷārikassa.",
  "anya-e0401n6:18_20": "‘‘Attūpanibandhā’’ti attena upanibandhā,",
  "anya-e0401n6:18_21": "paṭibaddhā.",
  "anya-e0401n6:19_0": "‘‘Kāmaṁ’’ti ekantena.",
  "anya-e0401n6:19_1": "Upasussatūti sambandho.",
  "anya-e0401n6:19_2": "‘‘Yantaṁ’’ti jhānābhiññā maggaphalanibbānaṁ.",
  "anya-e0401n6:19_3": "‘‘Vīriyassa saṇṭhānaṁ’’ti vīriyagamanassa vīriyappavattanassa nivattivasena saṇṭhānaṁ.",
  "anya-e0401n6:19_4": "Uppannasaddena atīta kilesāpi paccuppanna kilesāpi gahetabbā hontīti āha ‘‘asukasmiṁ kāle’’tiādiṁ.",
  "anya-e0401n6:19_5": "‘‘Mayhaṁ uppannaṁ’’ti idāni uppannaṁ.",
  "anya-e0401n6:19_6": "‘‘Pahānāyā’’ti ettha samucchedappahānaṁ adhippetanti āha ‘‘anuppāda dhammatāpādanakaraṇatthaṁ’’ti.",
  "anya-e0401n6:19_7": "Tattha anuppādo dhammo yesaṁ te anuppāda dhammā.",
  "anya-e0401n6:19_8": "Maggena pahīnā akusala dhammā.",
  "anya-e0401n6:19_9": "Anuppāda dhammānaṁ bhāvoti viggaho.",
  "anya-e0401n6:19_10": "Anuppāda dhammatāya āpādanaṁ.",
  "anya-e0401n6:19_11": "Tassa karaṇanti samāso.",
  "anya-e0401n6:19_12": "‘‘Disvā’’ti tesaṁ uppatti vatthuṁ disvā.",
  "anya-e0401n6:19_13": "‘‘Paccayasamucchindanatthaṁ’’ti paccayabhūtassa anusayassa samucchindanatthaṁ.",
  "anya-e0401n6:19_14": "‘‘Apattā’’ti idāni apattā.",
  "anya-e0401n6:19_15": "‘‘Samaye’’ti buddhuppādanavamakkhaṇe.",
  "anya-e0401n6:19_16": "‘‘Niyāmaṁ na okkamantī’’ti ariyamaggasaṅkhātaṁ sammattaniyāmaṁ na okkamanti.",
  "anya-e0401n6:19_17": "Nappavisanti.",
  "anya-e0401n6:19_18": "Kilesehi akopetabbatāya akuppo dhammo yassāti akuppa dhammo.",
  "anya-e0401n6:19_19": "Khīṇāsavo.",
  "anya-e0401n6:19_20": "Akuppa dhammassa bhāvo akuppa dhammatā.",
  "anya-e0401n6:19_21": "Arahatta phalaṁ.",
  "anya-e0401n6:19_22": "‘‘Tesaṁ’’ti kusalānaṁ dhammānaṁ.",
  "anya-e0401n6:19_23": "Uppannānaṁ pāpakānaṁ pahānāyāti katvā visuddhidhammesu yogaṁ karontassa anukkamena bhāvanā kamme matthakaṁ",
  "anya-e0401n6:19_24": "patteādito paṭṭhāya uppannāpi pāpakā pahiyyanti.",
  "anya-e0401n6:19_25": "Anuppannāpi nuppajjanti.",
  "anya-e0401n6:19_26": "Anuppannāpi kusalā uppajjanti.",
  "anya-e0401n6:19_27": "Uppannāpi bhāvanā pāripūriṁ gacchanti.",
  "anya-e0401n6:19_28": "Esanayo anuppannānaṁ pāpakānaṁ anuppādāyātiādīsu.",
  "anya-e0401n6:19_29": "Evaṁ catūsu mukhesu ekekamukhenapi sammā padahantassa sammappadhānaṁ catukiccasādhakaṁ hoti.",
  "anya-e0401n6:19_30": "Tenāha ‘‘etesu panā’’tiādiṁ.",
  "anya-e0401n6:20_0": "Abhivisiṭṭhena dhammavavatthānaññāṇena ñātabbā sabbe paramattha dhammā abhiññeyyā nāma.",
  "anya-e0401n6:20_1": "‘‘Abhiññā siddhī’’ti abhijānana kiccasiddhi.",
  "anya-e0401n6:20_2": "Visuṁ visuṁ paricchinditvā ñātabbā catusacca dhammā pariññeyyā nāma.",
  "anya-e0401n6:20_3": "Tehi dukkhasacca dhammā pariññātabbabhāvena,",
  "anya-e0401n6:20_4": "samudaya sacca dhammā pahātabba bhāvena,",
  "anya-e0401n6:20_5": "nirodhasacca dhammā sacchikātabba bhāvena,",
  "anya-e0401n6:20_6": "maggasacca dhammā bhāvetabbabhāvena ñātabbā hontīti.",
  "anya-e0401n6:20_7": "‘‘Bhāvanāsiddhī’’ti bhāvanākicca siddhi.",
  "anya-e0401n6:20_8": "Taṁ saṁyuttake imāyapāḷiyā sametīti sambandho.",
  "anya-e0401n6:20_9": "‘‘Vibhaṅge panā’’ti iddhipādavibhaṅge pana.",
  "anya-e0401n6:20_10": "‘‘Citta cetasikarāsī’’ti saha pubbabhāgabhāvanāya uttari manussa dhammapariyā pannā cittacetasikarāsi.",
  "anya-e0401n6:20_11": "‘‘Tattha panā’’ti vibhaṅge tasmiṁ uttaracūḷabhājanīye pana.",
  "anya-e0401n6:20_12": "‘‘Idhā’’ti imasmiṁ saṅgahe.",
  "anya-e0401n6:21_0": "Cattāri sotāpattiyaṅgāni nāma buddhe aveccappasādena samannāgato. Dhamme, saṅghe,",
  "anya-e0401n6:21_1": "ariyakantehi sīlehi samannāgatoti evaṁ vuttāni sotāpattiyā aṅgāni.",
  "anya-e0401n6:21_2": "Tattha ‘‘ariyakantehī’’ti ariyajanehi kāmitehi icchitehi.",
  "anya-e0401n6:21_3": "‘‘Sīlehī’’ti pañcaniccasīlehi,",
  "anya-e0401n6:21_4": "ājīvaṭṭhamakasīlehi vā.",
  "anya-e0401n6:21_5": "‘‘Sammappadhānesū’’ti sammappadhānakiccesu.",
  "anya-e0401n6:22_0": "‘‘Soḷasahikiccehī’’ti ekekasmiṁ sacce cattāri cattāri katvā soḷasahi kiccehi.",
  "anya-e0401n6:22_1": "Pīḷanaṭṭho,",
  "anya-e0401n6:22_2": "saṅkhataṭṭho,",
  "anya-e0401n6:22_3": "tiādīhi soḷasahi atthehīti vuttaṁ hoti.",
  "anya-e0401n6:22_4": "Te ca atthā parato āgamissanti.",
  "anya-e0401n6:22_5": "Sati eva sambojjhaṅgoti vuttaṁ,",
  "anya-e0401n6:22_6": "kathaṁ pana sāsati sambodhiṁ samuṭṭhāpetīti āha ‘‘sāhī’’tiādiṁ.",
  "anya-e0401n6:22_7": "Esanayo sesabojjhaṅgesupi.",
  "anya-e0401n6:22_8": "‘‘Tesu dhammesū’’ti ajjhattabahiddhā dhammesu.",
  "anya-e0401n6:22_9": "‘‘Līnasaṅkoca kosajjapakkhaṁ’’ti līnapakkha saṅkoca pakkhakosajja pakkhaṁ.",
  "anya-e0401n6:22_10": "Dhuraṁ suṭṭhu paggaṇhāti ukkhipatīti dhurasampaggaho.",
  "anya-e0401n6:22_11": "‘‘Dhuraṁ’’ti samathadhuraṁ vipassanā dhurañca.",
  "anya-e0401n6:22_12": "‘‘Arati ukkaṇṭha pakkhaṁ’’ti aratipakkha nibbidāpakkhaṁ.",
  "anya-e0401n6:22_13": "Dhamme rati dhammarati.",
  "anya-e0401n6:22_14": "Dhamme nandī dhammanandī.",
  "anya-e0401n6:22_15": "Dhamme ārāmo dhammārāmo.",
  "anya-e0401n6:22_16": "‘‘Sārambho’’ nāma cittavihesā.",
  "anya-e0401n6:22_17": "‘‘Daratho’’ nāma cittasantāpo.",
  "anya-e0401n6:22_18": "Samāhito bhikkhave yathābhūtaṁ pajānāti passatīti vacanato paññāya samādhi pādakatā pākaṭāti vuttaṁ ‘‘tassā’’tiādiṁ.",
  "anya-e0401n6:22_19": "Samaṁ avisamaṁ attano kiccaṁ vahati sīlenāti samavāhī.",
  "anya-e0401n6:22_20": "Samavāhissa bhāvo samavāhitā.",
  "anya-e0401n6:23_0": "Sā pana catubbidhā hotīti sambandho.",
  "anya-e0401n6:23_1": "‘‘Tassā’’ti sammādiṭṭhiyā.",
  "anya-e0401n6:23_2": "‘‘Visākhā’’ti ālapana padaṁ.",
  "anya-e0401n6:23_3": "‘Sabbe lokuttare hontī’ti vacanassa sambandhaṁ dassetuṁ ‘‘te pana sattatiṁsappabhedā’’tiādimāha.",
  "anya-e0401n6:23_4": "Na vaṭṭhānāni assa atthīti navaṭṭhānikaṁ.",
  "anya-e0401n6:23_5": "‘‘Lokuttare’’ti lokuttara citte.",
  "anya-e0401n6:23_6": "‘‘Attappaṭipakkhānaṁ’’ti attanā ujuppaṭipakkhānaṁ.",
  "anya-e0401n6:23_7": "‘‘Te’’ti bodhipakkhiya dhammā.",
  "anya-e0401n6:23_8": "Etena dassetīti sambandho.",
  "anya-e0401n6:23_9": "‘‘Kuṇḍaliyā’’ti evaṁ nāmakaṁ brāhmaṇaṁ ālapati.",
  "anya-e0401n6:23_10": "‘‘Vijjā’’ti maggavijjā.",
  "anya-e0401n6:23_11": "‘‘Vimuttī’’ti phalavimutti.",
  "anya-e0401n6:24_0": "Bodhipakkhiyānudīpanā niṭṭhitā.",
  "anya-e0401n6:25_0": "165.",
  "anya-e0401n6:25_1": "Sabbasaṅgahe.",
  "anya-e0401n6:25_2": "‘‘Pañcarāsayo’’ti pañcasaṅgahā.",
  "anya-e0401n6:25_3": "‘‘Atītādibhedabhinnānaṁ’’ti atītānāgatapaccuppannabhedena,",
  "anya-e0401n6:25_4": "ajjhatta bahiddhābhedena,",
  "anya-e0401n6:25_5": "oḷārikasukhumabhedena,",
  "anya-e0401n6:25_6": "hīnapaṇītabhedena,",
  "anya-e0401n6:25_7": "dūrasantikabhedena ca bhinnānaṁ.",
  "anya-e0401n6:25_8": "‘‘Upapatti dvāresū’’ti cakkhādi dvāresu.",
  "anya-e0401n6:25_9": "‘‘Kammadvāresū’’ti kāyakammadvārādīsu.",
  "anya-e0401n6:25_10": "‘‘Saṅgammā’’ti saṅgantvā.",
  "anya-e0401n6:25_11": "‘‘Samāgammā’’ti samāgantvā.",
  "anya-e0401n6:25_12": "‘‘Sādhāraṇa kiccāni vā’’ti sabbiriyāpathehi sādhāraṇa kiccānivā.",
  "anya-e0401n6:25_13": "Pāḷipāṭhe.",
  "anya-e0401n6:25_14": "‘‘Kiñcā’’ti kasmā ca.",
  "anya-e0401n6:25_15": "‘‘Saṅkhārevadethā’’ti saṅkhārā saṅkhārā iti tumhe vadetha.",
  "anya-e0401n6:25_16": "‘‘Kiñcasaṅkhataṁ’’ti katamañcasaṅkhataṁ.",
  "anya-e0401n6:25_17": "‘‘Rūpaṁ rūpatthāyā’’ti rūpavikatiyā jātatthāya rūpasaṅkhātaṁ saṅkhataṁ abhisaṅkharontīti attho.",
  "anya-e0401n6:25_18": "Upabhogo nāma vatthābharaṇādiko antobhogo.",
  "anya-e0401n6:25_19": "Paribhogo nāma mañcapīṭhādiko bāhira paribhogo.",
  "anya-e0401n6:25_20": "Samparāyikañca vedananti adhikāro.",
  "anya-e0401n6:25_21": "Sesesupi saññāsaṅkhāra viññāṇesupi.",
  "anya-e0401n6:25_22": "‘‘Dānaṁ dānatthāyā’’ti dānapuññassa jātatthāya.",
  "anya-e0401n6:25_23": "Dānasaṅkhātaṁ saṅkhataṁ abhisaṅkharontīti attho.",
  "anya-e0401n6:25_24": "Athavā dānassa attho dānattho.",
  "anya-e0401n6:25_25": "‘‘Attho’’ti ca vipākattho phalattho.",
  "anya-e0401n6:25_26": "Dānavipākatthāya dānaphalatthāya dānapuññasaṅkhātaṁ saṅkhataṁ abhisaṅkharontīti attho.",
  "anya-e0401n6:25_27": "Evaṁ sesesupi.",
  "anya-e0401n6:25_28": "‘‘Anuyuñjantā’’ti vipassanā kammaṁ anuyuñjantā.",
  "anya-e0401n6:25_29": "‘‘Tīsu dhammesu kilamissantī’’ti vuttaṁ.",
  "anya-e0401n6:25_30": "Kathaṁ kilamissantīti āha ‘‘tathāhi yathā’’tiādiṁ.",
  "anya-e0401n6:25_31": "‘‘Sukhasaññitesu paṭisaraṇesū’’ti cittasātacittarati paṭilābhatthāya paṭisaraṇesu ārammaṇesu.",
  "anya-e0401n6:25_32": "‘‘Tāhī’’ti sampattīhi.",
  "anya-e0401n6:25_33": "‘‘Tā’’ti sampattiyo.",
  "anya-e0401n6:25_34": "Paramukkaṁsagataṁ sukhasaññitaṭṭhānaṁ hoti.",
  "anya-e0401n6:25_35": "Tasmā tattha dukkhānupassanāñāṇaṁ atirekataraṁ pavattetabbanti tadatthāya bhagavā taṁ vedanaṁ visuṁ ekaṁ",
  "anya-e0401n6:25_36": "khandhaṁ katvā khandha desanaṁ desetīti adhippāyo.",
  "anya-e0401n6:25_37": "Esanayo paratthapi.",
  "anya-e0401n6:25_38": "‘‘Ñāṇasammataṁ’’ti puthujjanehi ñāṇasammataṁ.",
  "anya-e0401n6:25_39": "‘‘Paramaṁ attānaṁ katvā’’ti loke viññuttaṁ paramaṁ hoti,",
  "anya-e0401n6:25_40": "tasmā vijānanaṁ paramaṁ attānaṁ katvā.",
  "anya-e0401n6:25_41": "Sesamettha supākaṭaṁ.",
  "anya-e0401n6:25_42": "‘‘Yatthabhuñjatī’’tiādīsu.",
  "anya-e0401n6:25_43": "‘‘Yattha bhuñjatī’’ti yasmiṁ bhājane bhuñjati.",
  "anya-e0401n6:25_44": "‘‘Yañca bhuñjatī’’ti yañca bhojanaṁ bhuñjati.",
  "anya-e0401n6:25_45": "‘‘Yena ca bhuñjatī’’ti yenabyañjanena bhuñjati.",
  "anya-e0401n6:25_46": "‘‘Yo ca bhojako’’ti samīpe ṭhatvā parivesako.",
  "anya-e0401n6:25_47": "‘‘Yo ca bhuñjitā’’ti yo ca bhuñjako.",
  "anya-e0401n6:26_0": "‘‘Sāsavā evā’’ti sāsavā nāma lokiya dhammā.",
  "anya-e0401n6:26_1": "‘‘Ussāhantā viya hontī’’ti abyāpāra dhammattā vuttaṁ.",
  "anya-e0401n6:26_2": "Na hi abyāpāra dhammānaṁ ekantena ussāho nāma atthi.",
  "anya-e0401n6:26_3": "Phalassa nibbattanato phalatthāya ussāhantā viya hontīti.",
  "anya-e0401n6:26_4": "‘‘Ākiritvā’’ti pattharitvā.",
  "anya-e0401n6:26_5": "Sesamettha subodhaṁ.",
  "anya-e0401n6:27_0": "Dhātusaṅgahe.",
  "anya-e0401n6:27_1": "‘‘Attano eva sabhāvaṁ’’ti attano sabhāvaṁ evātipi yujjati.",
  "anya-e0401n6:27_2": "‘‘Īhā ca byāpāro cā’’ti abhikkamāmi paṭikkamāmītiādinā ussāho ca,",
  "anya-e0401n6:27_3": "abhikkamanādi byāpāro ca.",
  "anya-e0401n6:27_4": "‘‘Na ca jīvayogā’’ti na ca lokiyamahājana sammatena jīvena yuttā.",
  "anya-e0401n6:27_5": "Aṭṭhakathāpāṭhe.",
  "anya-e0401n6:27_6": "Yāyati etthāti yantaṁ.",
  "anya-e0401n6:27_7": "Udukkhalaṁ.",
  "anya-e0401n6:27_8": "Yanta cakkayaṭṭhināma yanta cakkasambandhā musalayaṭṭhi.",
  "anya-e0401n6:27_9": "Araṇī vuccati agginibbattakatthāya katā dve sāraghaṭikā.",
  "anya-e0401n6:27_10": "Adharāraṇī nāma heṭṭhāraṇī.",
  "anya-e0401n6:27_11": "Uttarāraṇī nāma uddhāraṇī.",
  "anya-e0401n6:27_12": "‘‘Sallamivā’’ti kaṇḍoviya.",
  "anya-e0401n6:27_13": "‘‘Sūlamivā’’ti vijjhanakadaṇḍo viya.",
  "anya-e0401n6:27_14": "‘‘Āturāviyā’’ti tehisallasūlehi viddhagilānā viya.",
  "anya-e0401n6:27_15": "Ayathābhucca nimittaṁ nāma ayathābhūtaṁ sattapuggalādinimittaṁ.",
  "anya-e0401n6:27_16": "‘‘Vanamigo viyā’’ti tiṇa rūpe purisa saññālābho vanamigo viya.",
  "anya-e0401n6:27_17": "‘‘Aṅgārakāsuyaṁ’’ti aṅgārakūpe.",
  "anya-e0401n6:27_18": "‘‘Nānāvidhupaddava nimittato’’ti nānāvidhānaṁ upaddavānaṁ uppatti paccayato.",
  "anya-e0401n6:27_19": "‘‘Vanamakkaṭo viyā’’ti vanavānaro viya.",
  "anya-e0401n6:27_20": "Dukkhena damanaṁ assāti duddamano.",
  "anya-e0401n6:27_21": "‘‘Assakhaḷuṅko’’ti duṭṭha asso.",
  "anya-e0401n6:27_22": "‘‘Yattha kāmanipātito’’ti yasmiṁ yasmiṁ ārammaṇe patituṁ kāmeti icchati,",
  "anya-e0401n6:27_23": "tattha tattha nipātito.",
  "anya-e0401n6:27_24": "Puggalavase avattitvā ārammaṇesu yadicchakaṁ nipatanatoti vuttaṁ hoti.",
  "anya-e0401n6:27_25": "‘‘Raṅganaṭo viyā’’ti samajjanaṭako viya.",
  "anya-e0401n6:27_26": "Sabhāvato atthi saṁvijjatīti santo.",
  "anya-e0401n6:27_27": "Tathoti ca avitathoti ca atthato ekaṁ.",
  "anya-e0401n6:27_28": "Añño pakāro aññathā.",
  "anya-e0401n6:27_29": "Natthi aññathā assāti anaññathaṁ.",
  "anya-e0401n6:27_30": "Dukkhadukkhaṁ nāma kāyikadukkha cetasikadukkhaṁ.",
  "anya-e0401n6:27_31": "‘‘Kammajānaṁ’’ti vipākakkhandhakaṭattā rūpānaṁ.",
  "anya-e0401n6:27_32": "‘‘Uppattiyā’’ti uppādatthāya.",
  "anya-e0401n6:27_33": "‘‘Pagevā’’ti puretarameva.",
  "anya-e0401n6:27_34": "‘‘Uppannaṁ pī’’ti uppannampi cakkhu.",
  "anya-e0401n6:27_35": "‘‘Pavattiyā’’ti santānaṭṭhitiyā ṭhitatthāya.",
  "anya-e0401n6:27_36": "‘‘Paṭijaggana dukkhaṁ’’ti sodhana dukkhaṁ.",
  "anya-e0401n6:27_37": "Paccayavekallaṁ nāma āhāracchedādikaṁ.",
  "anya-e0401n6:27_38": "Nanu bhijjanabhayena pageva paccayasampādanaṁ nāma saṅkhāra dukkhameva siyāti. Saccaṁ.",
  "anya-e0401n6:27_39": "Idha pana āsanne bhijjananimittaṁ disvā kataṁ paccaya sampādanaṁ adhippetaṁ.",
  "anya-e0401n6:27_40": "‘‘Rakkhāvaraṇagutti saṁvidhāna dukkhaṁ’’ti nānābhayato rakkhaṇassa ca āvaraṇassa ca gopanassa ca saṁvidhāna dukkhaṁ.",
  "anya-e0401n6:27_41": "‘‘Tadubhayena saheva sijjhatī’’ti kasigorakkhādīni saṅkhāra kammāni vā rakkhāvaraṇa guttisaṁ vidhānādīnivā",
  "anya-e0401n6:27_42": "karontānaṁ taṁ dukkha dukkhaṁ tadubhayena dukkhena saheva sijjhati.",
  "anya-e0401n6:27_43": "‘‘Tesaṁ’’ti cakkhādīni assādentānaṁ na muccatīti sambandho.",
  "anya-e0401n6:27_44": "‘‘Pavatti nirodha bhūtassā’’ti pavattiyā nirodha bhūtassa.",
  "anya-e0401n6:27_45": "‘‘Taṁ pajahati yevā’’ti taṁ lobhaṁ pajahatiyeva.",
  "anya-e0401n6:27_46": "‘‘Yāvadevā’’ti uparimapariyantena.",
  "anya-e0401n6:27_47": "Ahaṁ asmīti pavatto māno asmimāno.",
  "anya-e0401n6:27_48": "Tassa samugghātanaṁ samucchindanaṁ.",
  "anya-e0401n6:27_49": "‘‘Pariññātesū’’ti tesu anusayitāya taṇhāya pahānavasena pariññātesu.",
  "anya-e0401n6:27_50": "‘‘Tabbiparītena panā’’ti tato viparītena pana.",
  "anya-e0401n6:27_51": "Asmimānassa anadhiṭṭhānabhūtātiādi atthena.",
  "anya-e0401n6:27_52": "Anindriyabaddha dhammā nāma pathavīpabbatādīsu pavattā rūpa dhammā.",
  "anya-e0401n6:28_0": "Satte anukkaṇṭhamāne karotīti sambandho.",
  "anya-e0401n6:28_1": "‘‘Itī’’ti tasmā.",
  "anya-e0401n6:28_2": "Tesaṁ sattānaṁ.",
  "anya-e0401n6:28_3": "Dujjaho nāma natthi.",
  "anya-e0401n6:28_4": "Tasmā lobhoyeva dukkhasamudayāriya saccaṁ nāmāti yojanā.",
  "anya-e0401n6:28_5": "Sāsavā kusalā kusala dhammā samudayasaccaṁ nāma,",
  "anya-e0401n6:28_6": "dukkha saccabhūtānaṁ vipākakkhandhakaṭattā rūpānaṁ samudayattā saṁvaḍḍhanattā.",
  "anya-e0401n6:28_7": "Mūlabhāvena vuttā ‘avijjā paccayā saṅkhārā’tiādīsu.",
  "anya-e0401n6:28_8": "‘‘Nandanasadisaṁ’’ti devaloke nandanavanuyyānasadisaṁ.",
  "anya-e0401n6:28_9": "‘‘Ādipabhavapadhānabhūtā’’ti dukkha dhammānaṁ ādibhūtā,",
  "anya-e0401n6:28_10": "pabhavabhūtā,",
  "anya-e0401n6:28_11": "padhānabhūtā.",
  "anya-e0401n6:28_12": "‘‘Pabhavabhūtā’’ti ca pathamuppattihetubhūtā.",
  "anya-e0401n6:28_13": "‘‘Tesaṁ’’ti dukkha dhammānaṁ.",
  "anya-e0401n6:29_0": "‘‘Katthacī’’ti kāmabhavādīsu kismiñcibhave.",
  "anya-e0401n6:29_1": "‘‘Kadācī’’ti atītādikālesu kismiñcikāle.",
  "anya-e0401n6:29_2": "‘‘Kesañcī’’ti devamanussādīsu sattesu kesañcisattānaṁ.",
  "anya-e0401n6:29_3": "‘‘Kutocī’’ti bahūsukāraṇesu kutocikāraṇā.",
  "anya-e0401n6:29_4": "‘‘Kathañcī’’ti kenacipakārena.",
  "anya-e0401n6:30_0": "‘‘Pariṇāmo’’ti pariṇamanaṁ aññathābhāvo.",
  "anya-e0401n6:30_1": "‘‘Taṁ samaṅgīnaṁ’’ti tehi cakkhādīhi samaṅgīnaṁ puggalaṁ.",
  "anya-e0401n6:30_2": "Pāḷipāṭhe.",
  "anya-e0401n6:30_3": "‘‘Ādittaṁ’’tiādīpitaṁ samujjalantaṁ.",
  "anya-e0401n6:30_4": "‘‘Kenā’’ti karaṇabhūtena kena agginā.",
  "anya-e0401n6:30_5": "Tathā ariyamaggo vipariṇāma dhammo ca hoti.",
  "anya-e0401n6:30_6": "Sovipariṇāma dukkhenapi puggalaṁ bhusaṁ pīḷetiyevāti yojanā.",
  "anya-e0401n6:30_7": "‘‘Nā’’ti na siyā.",
  "anya-e0401n6:30_8": "Samanaṁ vūpasamanaṁ santi.",
  "anya-e0401n6:30_9": "Sabbadukkhānaṁ santīti viggaho.",
  "anya-e0401n6:30_10": "Sabbaṁ vaṭṭadukkhanti sambandho.",
  "anya-e0401n6:30_11": "‘‘Tassā’’ti ariyamaggassa.",
  "anya-e0401n6:30_12": "‘‘Pavattamānā cā’’ti santatiṭhitivasena vattamānā ca.",
  "anya-e0401n6:30_13": "‘‘Mahantaṁ pariḷāhadukkhaṁ’’ti soka parideva dukkha domanassupāyāsānaṁ pavattivasena mahantaṁ pariḷāha dukkhaṁ.",
  "anya-e0401n6:30_14": "Nibbānassa anuppāda dhammattā uppādatthāya paccayo nāma natthi.",
  "anya-e0401n6:30_15": "Tathā appavatti dhammattā pavattatthāya.",
  "anya-e0401n6:30_16": "Sampāpuṇitabbattā pana sampāpanatthāya paccayo atthīti āha ‘‘sampāpaka paccayābhisaṅkharaṇalesaṁ’’ti.",
  "anya-e0401n6:30_17": "Sampāpaka paccayā nāma ariya maggo ca tassa paccayā ca.",
  "anya-e0401n6:31_0": "Bhavābhavasaṅkhātassa dukkhakkhandhassāti sambandho.",
  "anya-e0401n6:31_1": "‘‘Tadevā’’ti taṁ dukkhakkhandhaṁ eva.",
  "anya-e0401n6:31_2": "Anupunappunaṁ padīyate anuppadānaṁ.",
  "anya-e0401n6:31_3": "‘‘Niyyātanaṭṭho’’ti handa gaṇhāti sampaṭicchāpanaṭṭho.",
  "anya-e0401n6:31_4": "‘‘Tato’’ti vaṭṭadukkhato.",
  "anya-e0401n6:31_5": "‘‘Muccanūpāyassā’’ti dānasīlanikkhamādikassa muccanūpāyassa.",
  "anya-e0401n6:31_6": "‘‘Nissaraṇavimuttiyā’’ti nissaraṇa saṅkhātāya vimuttiyā.",
  "anya-e0401n6:31_7": "Saṅkhāra dukkhassa abhāvaṭṭho.",
  "anya-e0401n6:31_8": "‘‘Sammohassa pahānavasenā’’ti etena idha dassanaṁ nāma ito paṭṭhāya catūsu saccesu asammoha bhāvapatti eva ālokapatti eva vuccatīti dasseti .",
  "anya-e0401n6:31_9": "Tenāha ‘‘paṭivijjhanaṭṭho’’ti.",
  "anya-e0401n6:31_10": "Taṇhāya dāso taṇhādāso.",
  "anya-e0401n6:31_11": "Taṇhādāsassa bhāvo taṇhādāsabyaṁ.",
  "anya-e0401n6:31_12": "Paravasato vimuttivasena aparādhīnatā bhujissabhāvo nāma.",
  "anya-e0401n6:31_13": "Lokiyamaggāhi taṇhādāsabyaṁ nātivattanti.",
  "anya-e0401n6:31_14": "Taṇhāvasaṁ pūrayamānā taṇhāvisayaṁ vipākaṁ janenti.",
  "anya-e0401n6:31_15": "Ayaṁ ariyamaggo eva taṇhāya anārammaṇabhāve ṭhito taṇhādāsabyaṁ ativattati.",
  "anya-e0401n6:31_16": "Taṇhāvasaṁ bhindamāno taṇhāya avisayaṁ vipākaṁ janetīti adhippāyo.",
  "anya-e0401n6:31_17": "‘‘Dukkhamuttiyā’’ti dukkhato muccanatthaṁ.",
  "anya-e0401n6:31_18": "‘‘Dukkhavaḍḍhiyā’’ti dukkhavaḍḍhanatthāya.",
  "anya-e0401n6:31_19": "Sesamettha subodhaṁ.",
  "anya-e0401n6:31_20": "‘‘Khayavirāga nirodha bhūto’’ti ettha kilesa dhammānaṁ virajjanaṁ vigamanaṁ antaradhānaṁ virāgo.",
  "anya-e0401n6:31_21": "‘‘Anattha padānī’’ti ahitapadāni.",
  "anya-e0401n6:31_22": "Bhayādīnavapadāni.",
  "anya-e0401n6:31_23": "‘‘Dvārachakkaārammaṇa chakkabhedenā’’ti dvārachakkaṁ dhammārammaṇaṁ hoti. Evaṁsati,",
  "anya-e0401n6:31_24": "ārammaṇa chakkanti na vattabbaṁ dhammārammaṇassa visuṁ gahitattāti.",
  "anya-e0401n6:31_25": "No navattabbaṁ. Kasmā,",
  "anya-e0401n6:31_26": "dvārachakkato sesānaṁ dhammārammaṇānaṁ atthitāyāti dassetuṁ ‘‘ettha cā’’tiādi vuttaṁ.",
  "anya-e0401n6:31_27": "Parato ‘‘etthapī’’tiādīsu esevanayoti.",
  "anya-e0401n6:32_0": "Sabbasaṅgahānudīpanā niṭṭhitā.",
  "anya-e0401n6:33_0": "Samuccayasaṅgahadīpaniyāanudīpanā niṭṭhitā."
}