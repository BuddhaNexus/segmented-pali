{
  "anya-e0808n5:0_0": "6. Chappaccayavibhāga-chaṭṭhapariccheda",
  "anya-e0808n5:1_0": "Patthāranaya",
  "anya-e0808n5:2_0": "129.",
  "anya-e0808n5:3_0": " Patthāre sabbage pāde, pubbagā’dho lpa’re samā;",
  "anya-e0808n5:4_0": "Pubbe garu te ca mime, kattabbā yāva sabbalā.",
  "anya-e0808n5:5_0": "Naṭṭhanaya",
  "anya-e0808n5:6_0": "130.",
  "anya-e0808n5:7_0": " Naṭṭhassa yo bhaveyya’ṅko, tasmiṁ lo’ddhikate same;",
  "anya-e0808n5:8_0": "Visame tve’kasahite, bhaveyya’ddhikate garu.",
  "anya-e0808n5:9_0": "Uddiṭṭhanaya",
  "anya-e0808n5:10_0": "131.",
  "anya-e0808n5:11_0": " Ekā’dinukkamena’ṅke, pubbādho dviguṇe likhe;",
  "anya-e0808n5:12_0": "Missakehi lahuṭṭhehi, se’kehu’ddiṭṭhakaṁ bhave.",
  "anya-e0808n5:13_0": "Sabbagalakriyanaya",
  "anya-e0808n5:14_0": "132.",
  "anya-e0808n5:15_0": " Vutta’kkhara samā saṅkhyā, likkhya seko’parū’pari;",
  "anya-e0808n5:16_0": "Ekekahīna mekādi, nu’ṭṭhāne sabbagādikaṁ.",
  "anya-e0808n5:17_0": "Vuttasaṅkhyānaya",
  "anya-e0808n5:18_0": "133.",
  "anya-e0808n5:19_0": " Garukriyā’ṅka sandohe, bhave saṅkhyā vimissite;",
  "anya-e0808n5:20_0": "Uddiṭṭha’ṅka samāhāro, se’ko ve’maṁ samā’naye.",
  "anya-e0808n5:21_0": "Vuttaaddhānaya",
  "anya-e0808n5:22_0": "134.",
  "anya-e0808n5:23_0": " Saṅkhyeva dviguṇe’kūnā, vitthārā’yāmasambhavā;",
  "anya-e0808n5:24_0": "Vuttassa’ddha’ntarānañca, garulānañca aṅgulaṁ.",
  "anya-e0808n5:25_0": "Iti vuttodaye chandasi chappaccayavibhāgo nāma",
  "anya-e0808n5:26_0": "Chaṭṭho paricchedo.",
  "anya-e0808n5:27_0": "Nigamana",
  "anya-e0808n5:28_0": "135.",
  "anya-e0808n5:29_0": " Sela’ntarā’yatana vāsika sīla’tthera,",
  "anya-e0808n5:30_0": "Pādo garu gguṇagaru jjayataṁ mame’so;",
  "anya-e0808n5:31_0": "Yassa ppabhāva’mavalamba maye’disopi,",
  "anya-e0808n5:32_0": "Sampādito’bhimata siddhikaro parattho.",
  "anya-e0808n5:33_0": "136.",
  "anya-e0808n5:34_0": " Parattha sampādanato, puññenā’dhigatena’haṁ;",
  "anya-e0808n5:35_0": "Parattha sampādanako, bhaveyyaṁ jāti jātiyaṁ.",
  "anya-e0808n5:36_0": "137.",
  "anya-e0808n5:37_0": " Avalokita mattena, yathā chappaccayā mayā;",
  "anya-e0808n5:38_0": "Sādhitā sādhayante’va, micchitatthampi pāṇinoti.",
  "anya-e0808n5:39_0": "Iti saṅgharakkhitamahāsāmittherena viracitaṁ",
  "anya-e0808n5:40_0": "Vuttodayappakaraṇaṁ pariniṭṭhitaṁ."
}