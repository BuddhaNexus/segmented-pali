{
  "vv8:0.1_0": "Vimānavatthu",
  "vv8:0.2_0": "Itthivimāna",
  "vv8:0.3_0": "Pīṭhavagga",
  "vv8:0.4_0": "8. Tatiyanāvāvimānavatthu",
  "vv8:1.1_0": "“Suvaṇṇacchadanaṁ nāvaṁ,",
  "vv8:1.2_0": "Nāri āruyha tiṭṭhasi;",
  "vv8:1.3_0": "Ogāhasi pokkharaṇiṁ,",
  "vv8:1.4_0": "Padmaṁ chindasi pāṇinā.",
  "vv8:2.1_0": "Kūṭāgārā nivesā te,",
  "vv8:2.2_0": "vibhattā bhāgaso mitā;",
  "vv8:2.3_0": "Daddallamānā ābhanti,",
  "vv8:2.4_0": "samantā caturo disā.",
  "vv8:3.1_0": "Kena tetādiso vaṇṇo,",
  "vv8:3.2_0": "kena te idha mijjhati;",
  "vv8:3.3_0": "Uppajjanti ca te bhogā,",
  "vv8:3.4_0": "ye keci manaso piyā.",
  "vv8:4.1_0": "Pucchāmi taṁ devi mahānubhāve,",
  "vv8:4.2_0": "Manussabhūtā kimakāsi puññaṁ;",
  "vv8:4.3_0": "Kenāsi evaṁ jalitānubhāvā,",
  "vv8:4.4_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv8:5.1_0": "Sā devatā attamanā,",
  "vv8:5.2_0": "sambuddheneva pucchitā;",
  "vv8:5.3_0": "Pañhaṁ puṭṭhā viyākāsi,",
  "vv8:5.4_0": "yassa kammassidaṁ phalaṁ.",
  "vv8:6.1_0": "“Ahaṁ manussesu manussabhūtā,",
  "vv8:6.2_0": "Purimāya jātiyā manussaloke;",
  "vv8:6.3_0": "Disvāna bhikkhū tasite kilante,",
  "vv8:6.4_0": "Uṭṭhāya pātuṁ udakaṁ adāsiṁ.",
  "vv8:7.1_0": "Yo ve kilantāna pipāsitānaṁ,",
  "vv8:7.2_0": "Uṭṭhāya pātuṁ udakaṁ dadāti;",
  "vv8:7.3_0": "Sītodakā tassa bhavanti najjo,",
  "vv8:7.4_0": "Pahūtamalyā bahupuṇḍarīkā.",
  "vv8:8.1_0": "Taṁ āpagā anupariyanti sabbadā,",
  "vv8:8.2_0": "Sītodakā vālukasanthatā nadī;",
  "vv8:8.3_0": "Ambā ca sālā tilakā ca jambuyo,",
  "vv8:8.4_0": "Uddālakā pāṭaliyo ca phullā.",
  "vv8:9.1_0": "Taṁ bhūmibhāgehi upetarūpaṁ,",
  "vv8:9.2_0": "Vimānaseṭṭhaṁ bhusa sobhamānaṁ;",
  "vv8:9.3_0": "Tassīdha kammassa ayaṁ vipāko,",
  "vv8:9.4_0": "Etādisaṁ puññakatā labhanti.",
  "vv8:10.1_0": "Kūṭāgārā nivesā me,",
  "vv8:10.2_0": "vibhattā bhāgaso mitā;",
  "vv8:10.3_0": "Daddallamānā ābhanti,",
  "vv8:10.4_0": "samantā caturo disā.",
  "vv8:11.1_0": "Tena metādiso vaṇṇo,",
  "vv8:11.2_0": "tena me idha mijjhati;",
  "vv8:11.3_0": "Uppajjanti ca me bhogā,",
  "vv8:11.4_0": "ye keci manaso piyā.",
  "vv8:12.1_0": "Akkhāmi te buddha mahānubhāva,",
  "vv8:12.2_0": "Manussabhūtā yamakāsi puññaṁ;",
  "vv8:12.3_0": "Tenamhi evaṁ jalitānubhāvā,",
  "vv8:12.4_0": "Vaṇṇo ca me sabbadisā pabhāsati;",
  "vv8:12.5_0": "Etassa kammassa phalaṁ mamedaṁ,",
  "vv8:12.6_0": "Atthāya buddho udakaṁ apāyī”ti.",
  "vv8:13.1_0": "Tatiyanāvāvimānaṁ aṭṭhamaṁ."
}