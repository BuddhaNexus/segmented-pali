{
  "mil3.6.10:0.1_0": "Milindapañha",
  "mil3.6.10:0.2_0": "Sativagga",
  "mil3.6.10:0.3_0": "10. Cirakatasaraṇapañha",
  "mil3.6.10:1.1_0": "Rājā āha—",
  "mil3.6.10:1.2_0": "“bhante nāgasena, kena atītaṁ cirakataṁ saratī”ti?",
  "mil3.6.10:1.3_0": "“Satiyā, mahārājā”ti.",
  "mil3.6.10:1.4_0": "“Nanu, bhante nāgasena, cittena sarati no satiyā”ti?",
  "mil3.6.10:1.5_0": "“Abhijānāsi nu tvaṁ, mahārāja,",
  "mil3.6.10:1.5_1": "kiñcideva karaṇīyaṁ katvā pamuṭṭhan”ti?",
  "mil3.6.10:1.6_0": "“Āma, bhante”ti.",
  "mil3.6.10:1.7_0": "“Kiṁ nu kho tvaṁ, mahārāja,",
  "mil3.6.10:1.7_1": "tasmiṁ samaye acittako ahosī”ti?",
  "mil3.6.10:1.8_0": "“Na hi, bhante, sati tasmiṁ samaye nāhosī”ti.",
  "mil3.6.10:1.9_0": "“Atha kasmā tvaṁ, mahārāja, evamāha—",
  "mil3.6.10:1.10_0": "‘cittena sarati, no satiyā’”ti?",
  "mil3.6.10:2.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.6.10:3.1_0": "Cirakatasaraṇapañho dasamo."
}