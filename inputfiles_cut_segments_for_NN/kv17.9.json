{
  "kv17.9:0.1_0": "Kathāvatthu",
  "kv17.9:0.2_0": "Mahāpaṇṇāsaka",
  "kv17.9:0.3_0": "Sattarasamavagga",
  "kv17.9:0.4_0": "Navattabbaṁsaṅghassadinnaṁmahapphalantikathā",
  "kv17.9:1.1_0": "Na vattabbaṁ—",
  "kv17.9:1.2_0": "“saṅghassa dinnaṁ mahapphalan”ti?",
  "kv17.9:1.3_0": "Āmantā.",
  "kv17.9:1.4_0": "Nanu saṅgho āhuneyyo pāhuneyyo dakkhiṇeyyo añjalikaraṇīyo anuttaraṁ puññakkhettaṁ lokassāti?",
  "kv17.9:1.5_0": "Āmantā.",
  "kv17.9:1.6_0": "Hañci saṅgho āhuneyyo …pe… anuttaraṁ puññakkhettaṁ lokassa, tena vata re vattabbe—",
  "kv17.9:1.7_0": "“saṅghassa dinnaṁ mahapphalan”ti.",
  "kv17.9:2.1_0": "Na vattabbaṁ—",
  "kv17.9:2.2_0": "“saṅghassa dinnaṁ mahapphalan”ti?",
  "kv17.9:2.3_0": "Āmantā.",
  "kv17.9:2.4_0": "Nanu cattāro purisayugā aṭṭha purisapuggalā dakkhiṇeyyā vuttā bhagavatāti?",
  "kv17.9:2.5_0": "Āmantā.",
  "kv17.9:2.6_0": "Hañci cattāro purisayugā aṭṭha purisapuggalā dakkhiṇeyyā vuttā bhagavatā, tena vata re vattabbe—",
  "kv17.9:2.7_0": "“saṅghassa dinnaṁ mahapphalan”ti.",
  "kv17.9:3.1_0": "Na vattabbaṁ—",
  "kv17.9:3.2_0": "“saṅghassa dinnaṁ mahapphalan”ti?",
  "kv17.9:3.3_0": "Āmantā.",
  "kv17.9:3.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv17.9:3.5_0": "“saṅghe, gotami, dehi,",
  "kv17.9:3.5_1": "saṅghe te dinne ahañceva pūjito bhavissāmi saṅgho cā”ti.",
  "kv17.9:3.6_0": "Attheva suttantoti?",
  "kv17.9:3.7_0": "Āmantā.",
  "kv17.9:3.8_0": "Tena hi saṅghassa dinnaṁ mahapphalanti.",
  "kv17.9:4.1_0": "Na vattabbaṁ—",
  "kv17.9:4.2_0": "“saṅghassa dinnaṁ mahapphalan”ti?",
  "kv17.9:4.3_0": "Āmantā.",
  "kv17.9:4.4_0": "Nanu sakko devānamindo bhagavantaṁ etadavoca—",
  "kv17.9:5.1_0": "“Yajamānānaṁ manussānaṁ,",
  "kv17.9:5.2_0": "puññapekkhāna pāṇinaṁ;",
  "kv17.9:5.3_0": "Karotaṁ opadhikaṁ puññaṁ,",
  "kv17.9:5.4_0": "kattha dinnaṁ mahapphalan”ti.",
  "kv17.9:6.1_0": "“Cattāro ca paṭipannā,",
  "kv17.9:6.2_0": "cattāro ca phale ṭhitā;",
  "kv17.9:6.3_0": "Esa saṅgho ujubhūto,",
  "kv17.9:6.4_0": "paññāsīlasamāhito.",
  "kv17.9:7.1_0": "Yajamānānaṁ manussānaṁ,",
  "kv17.9:7.2_0": "puññapekkhāna pāṇinaṁ;",
  "kv17.9:7.3_0": "Karotaṁ opadhikaṁ puññaṁ,",
  "kv17.9:7.4_0": "saṅghe dinnaṁ mahapphalanti.",
  "kv17.9:8.1_0": "Eso hi saṅgho vipulo mahaggato,",
  "kv17.9:8.2_0": "Esappameyyo udadhīva sāgaro;",
  "kv17.9:8.3_0": "Ete hi seṭṭhā naravīrasāvakā,",
  "kv17.9:8.4_0": "Pabhaṅkarā dhammamudīrayanti.",
  "kv17.9:9.1_0": "Tesaṁ sudinnaṁ suhutaṁ suyiṭṭhaṁ,",
  "kv17.9:9.2_0": "Ye saṅghamuddissa dadanti dānaṁ;",
  "kv17.9:9.3_0": "Sā dakkhiṇā saṅghagatā patiṭṭhitā,",
  "kv17.9:9.4_0": "Mahapphalā lokavidūna vaṇṇitā.",
  "kv17.9:10.1_0": "Etādisaṁ yaññamanussarantā,",
  "kv17.9:10.2_0": "Ye vedajātā vicaranti loke;",
  "kv17.9:10.3_0": "Vineyya maccheramalaṁ samūlaṁ,",
  "kv17.9:10.4_0": "Aninditā saggamupenti ṭhānan”ti.",
  "kv17.9:11.1_0": "Attheva suttantoti?",
  "kv17.9:11.2_0": "Āmantā.",
  "kv17.9:11.3_0": "Tena hi saṅghassa dinnaṁ mahapphalanti.",
  "kv17.9:11.4_0": "Na vattabbaṁ saṅghassa dinnaṁ mahapphalantikathā niṭṭhitā."
}