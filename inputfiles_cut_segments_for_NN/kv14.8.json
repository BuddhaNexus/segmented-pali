{
  "kv14.8:0.1_0": "Kathāvatthu",
  "kv14.8:0.2_0": "Mahāpaṇṇāsaka",
  "kv14.8:0.3_0": "Cuddasamavagga",
  "kv14.8:0.4_0": "Abyākatakathā",
  "kv14.8:1.1_0": "Diṭṭhigataṁ abyākatanti?",
  "kv14.8:1.2_0": "Āmantā.",
  "kv14.8:1.3_0": "Vipākābyākataṁ kiriyābyākataṁ rūpaṁ nibbānaṁ cakkhāyatanaṁ …pe…",
  "kv14.8:1.4_0": "phoṭṭhabbāyatananti?",
  "kv14.8:1.5_0": "Na hevaṁ vattabbe …pe…",
  "kv14.8:1.6_0": "diṭṭhigataṁ abyākatanti?",
  "kv14.8:1.7_0": "Āmantā.",
  "kv14.8:1.8_0": "Diṭṭhigatasampayutto phasso abyākatoti?",
  "kv14.8:1.9_0": "Na hevaṁ vattabbe …pe…",
  "kv14.8:1.10_0": "diṭṭhigataṁ abyākatanti?",
  "kv14.8:1.11_0": "Āmantā.",
  "kv14.8:1.12_0": "Diṭṭhigatasampayuttā vedanā …pe…",
  "kv14.8:1.13_0": "saññā …pe…",
  "kv14.8:1.14_0": "cetanā …pe…",
  "kv14.8:1.15_0": "cittaṁ abyākatanti?",
  "kv14.8:1.16_0": "Na hevaṁ vattabbe …pe….",
  "kv14.8:2.1_0": "Diṭṭhigatasampayutto phasso akusaloti?",
  "kv14.8:2.2_0": "Āmantā.",
  "kv14.8:2.3_0": "Diṭṭhigataṁ akusalanti?",
  "kv14.8:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv14.8:2.5_0": "diṭṭhigatasampayuttā vedanā saññā cetanā cittaṁ akusalanti?",
  "kv14.8:2.6_0": "Āmantā.",
  "kv14.8:2.7_0": "Diṭṭhigataṁ akusalanti?",
  "kv14.8:2.8_0": "Na hevaṁ vattabbe …pe….",
  "kv14.8:3.1_0": "Diṭṭhigataṁ abyākatanti?",
  "kv14.8:3.2_0": "Āmantā.",
  "kv14.8:3.3_0": "Aphalaṁ avipākanti?",
  "kv14.8:3.4_0": "Na hevaṁ vattabbe …pe…",
  "kv14.8:3.5_0": "nanu saphalaṁ savipākanti?",
  "kv14.8:3.6_0": "Āmantā.",
  "kv14.8:3.7_0": "Hañci saphalaṁ savipākaṁ, no ca vata re vattabbe—",
  "kv14.8:3.8_0": "“diṭṭhigataṁ abyākatan”ti.",
  "kv14.8:4.1_0": "Diṭṭhigataṁ abyākatanti?",
  "kv14.8:4.2_0": "Āmantā.",
  "kv14.8:4.3_0": "Nanu micchādiṭṭhiparamāni vajjāni vuttāni bhagavatāti?",
  "kv14.8:4.4_0": "Āmantā.",
  "kv14.8:4.5_0": "Hañci micchādiṭṭhiparamāni vajjāni vuttāni bhagavatā, no ca vata re vattabbe—",
  "kv14.8:4.6_0": "“diṭṭhigataṁ abyākatan”ti.",
  "kv14.8:5.1_0": "Diṭṭhigataṁ abyākatanti?",
  "kv14.8:5.2_0": "Āmantā.",
  "kv14.8:5.3_0": "Nanu vuttaṁ bhagavatā—",
  "kv14.8:5.4_0": "“micchādiṭṭhi kho, vaccha, akusalā, sammādiṭṭhi kusalā”ti.",
  "kv14.8:5.5_0": "Attheva suttantoti?",
  "kv14.8:5.6_0": "Āmantā.",
  "kv14.8:5.7_0": "Tena hi na vattabbaṁ—",
  "kv14.8:5.8_0": "“diṭṭhigataṁ abyākatan”ti.",
  "kv14.8:6.1_0": "Diṭṭhigataṁ abyākatanti?",
  "kv14.8:6.2_0": "Āmantā.",
  "kv14.8:6.3_0": "Nanu vuttaṁ bhagavatā—",
  "kv14.8:6.4_0": "“micchādiṭṭhissa kho ahaṁ, puṇṇa, dvinnaṁ gatīnaṁ aññataraṁ gatiṁ vadāmi—",
  "kv14.8:6.5_0": "nirayaṁ vā tiracchānayoniṁ vā”ti.",
  "kv14.8:6.6_0": "Attheva suttantoti?",
  "kv14.8:6.7_0": "Āmantā.",
  "kv14.8:6.8_0": "Tena hi na vattabbaṁ—",
  "kv14.8:6.9_0": "“diṭṭhigataṁ abyākatan”ti.",
  "kv14.8:7.1_0": "Na vattabbaṁ—",
  "kv14.8:7.2_0": "“diṭṭhigataṁ abyākatan”ti?",
  "kv14.8:7.3_0": "Āmantā.",
  "kv14.8:7.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv14.8:7.5_0": "“‘sassato loko’ti kho, vaccha, abyākatametaṁ,",
  "kv14.8:7.6_0": "‘asassato loko’ti kho, vaccha, abyākatametaṁ,",
  "kv14.8:7.7_0": "‘antavā loko’ti kho, vaccha, abyākatametaṁ,",
  "kv14.8:7.8_0": "‘anantavā loko’ti kho, vaccha …pe…",
  "kv14.8:7.9_0": "‘taṁ jīvaṁ taṁ sarīran’ti kho, vaccha …pe…",
  "kv14.8:7.10_0": "‘aññaṁ jīvaṁ aññaṁ sarīran’ti kho, vaccha …pe…",
  "kv14.8:7.11_0": "‘hoti tathāgato paraṁ maraṇā’ti kho, vaccha …pe…",
  "kv14.8:7.12_0": "‘na hoti tathāgato paraṁ maraṇā’ti kho, vaccha …pe…",
  "kv14.8:7.13_0": "‘hoti ca na ca hoti tathāgato paraṁ maraṇā’ti kho, vaccha …pe…",
  "kv14.8:7.14_0": "‘neva hoti na na hoti tathāgato paraṁ maraṇā’ti kho, vaccha, abyākatametan”ti.",
  "kv14.8:7.15_0": "Attheva suttantoti?",
  "kv14.8:7.16_0": "Āmantā.",
  "kv14.8:7.17_0": "Tena hi diṭṭhigataṁ abyākatanti.",
  "kv14.8:8.1_0": "Diṭṭhigataṁ abyākatanti?",
  "kv14.8:8.2_0": "Āmantā.",
  "kv14.8:8.3_0": "Nanu vuttaṁ bhagavatā—",
  "kv14.8:8.4_0": "“micchādiṭṭhikassa,",
  "kv14.8:8.4_1": "bhikkhave,",
  "kv14.8:8.4_2": "purisapuggalassa yañceva kāyakammaṁ yathādiṭṭhisamattaṁ samādinnaṁ,",
  "kv14.8:8.4_3": "yañca vacīkammaṁ …pe…",
  "kv14.8:8.4_4": "yañca manokammaṁ,",
  "kv14.8:8.4_5": "yā ca cetanā,",
  "kv14.8:8.4_6": "yā ca patthanā,",
  "kv14.8:8.4_7": "yo ca paṇidhi,",
  "kv14.8:8.4_8": "ye ca saṅkhārā,",
  "kv14.8:8.4_9": "sabbe te dhammā aniṭṭhāya akantāya amanāpāya ahitāya dukkhāya saṁvattantī”ti.",
  "kv14.8:8.5_0": "Attheva suttantoti?",
  "kv14.8:8.6_0": "Āmantā.",
  "kv14.8:8.7_0": "Tena hi na vattabbaṁ—",
  "kv14.8:8.8_0": "“diṭṭhigataṁ abyākatan”ti.",
  "kv14.8:8.9_0": "Abyākatakathā niṭṭhitā."
}