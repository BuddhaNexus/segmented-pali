{
  "mil7.3.6:0.1_0": "Milindapañha",
  "mil7.3.6:0.2_0": "Opammakathāpañha",
  "mil7.3.6:0.3_0": "Samuddavagga",
  "mil7.3.6:0.4_0": "6. Nāvālagganakaṅgapañha",
  "mil7.3.6:1.1_0": "“Bhante nāgasena,",
  "mil7.3.6:1.1_1": "‘nāvālagganakassa dve aṅgāni gahetabbānī’ti yaṁ vadesi,",
  "mil7.3.6:1.1_2": "katamāni tāni dve aṅgāni gahetabbānī”ti?",
  "mil7.3.6:1.2_0": "“Yathā, mahārāja,",
  "mil7.3.6:1.2_1": "nāvālagganakaṁ bahuūmijālākulavikkhobhitasalilatale mahatimahāsamudde nāvaṁ laggeti ṭhapeti,",
  "mil7.3.6:1.2_2": "na deti disāvidisaṁ harituṁ;",
  "mil7.3.6:1.3_0": "evameva kho, mahārāja,",
  "mil7.3.6:1.3_1": "yoginā yogāvacarena rāgadosamohūmijāle mahatimahāvitakkasampahāre cittaṁ laggetabbaṁ,",
  "mil7.3.6:1.3_2": "na dātabbaṁ disāvidisaṁ harituṁ.",
  "mil7.3.6:1.4_0": "Idaṁ, mahārāja, nāvālagganakassa paṭhamaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.3.6:2.1_0": "Puna caparaṁ, mahārāja,",
  "mil7.3.6:2.1_1": "nāvālagganakaṁ na plavati visīdati,",
  "mil7.3.6:2.1_2": "hatthasatepi udake nāvaṁ laggeti ṭhānamupaneti;",
  "mil7.3.6:2.2_0": "evameva kho, mahārāja,",
  "mil7.3.6:2.2_1": "yoginā yogāvacarena lābhayasasakkāramānanavandanapūjanaapacitīsu lābhaggayasaggepi na plavitabbaṁ,",
  "mil7.3.6:2.2_2": "sarīrayāpanamattake yeva cittaṁ ṭhapetabbaṁ.",
  "mil7.3.6:2.3_0": "Idaṁ, mahārāja, nāvālagganakassa dutiyaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.3.6:2.4_0": "Bhāsitampetaṁ, mahārāja, therena sāriputtena dhammasenāpatinā—",
  "mil7.3.6:3.1_0": "‘Yathā samudde lagganakaṁ,",
  "mil7.3.6:3.2_0": "na plavati visīdati;",
  "mil7.3.6:3.3_0": "Tatheva lābhasakkāre,",
  "mil7.3.6:3.4_0": "mā plavatha visīdathā’”ti.",
  "mil7.3.6:4.1_0": "Nāvālagganakaṅgapañho chaṭṭho."
}