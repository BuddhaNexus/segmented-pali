{
  "patthana17.19:0.1_0": "Paṭṭhānapakaraṇa",
  "patthana17.19:0.2_0": "Dhammānulomapaccanīya (3), Tikatikapaṭṭhānapāḷi (5)",
  "patthana17.19:0.3_0": "17.19. Kusalattika, Ajjhattattikadvaya",
  "patthana17.19:1.1_0": "Kusalo ajjhatto dhammo naajjhattassa nakusalassa dhammassa ārammaṇapaccayena paccayo.",
  "patthana17.19:1.1_1": "(Saṅkhittaṁ.) Ārammaṇe aṭṭhārasa,",
  "patthana17.19:1.1_2": "adhipatiyā nava,",
  "patthana17.19:1.1_3": "upanissaye aṭṭhārasa,",
  "patthana17.19:1.1_4": "purejāte cha,",
  "patthana17.19:1.1_5": "āhāre tīṇi,",
  "patthana17.19:1.1_6": "atthiyā avigate cha.",
  "patthana17.19:2.1_0": "Kusalo bahiddhā dhammo nabahiddhā nakusalassa dhammassa ārammaṇapaccayena paccayo.",
  "patthana17.19:2.1_1": "(Saṅkhittaṁ.) Ārammaṇe aṭṭhārasa,",
  "patthana17.19:2.1_2": "adhipatiyā pannarasa,",
  "patthana17.19:2.1_3": "upanissaye aṭṭhārasa,",
  "patthana17.19:2.1_4": "purejāte cha,",
  "patthana17.19:2.1_5": "āhāre tīṇi,",
  "patthana17.19:2.1_6": "atthiyā avigate cha.",
  "patthana17.19:3.1_0": "Kusalaṁ ajjhattārammaṇaṁ dhammaṁ paṭicca nakusalo naajjhattārammaṇo dhammo uppajjati hetupaccayā.",
  "patthana17.19:3.1_1": "(Saṅkhittaṁ.) Hetuyā nava,",
  "patthana17.19:3.1_2": "adhipatiyā nava …pe…",
  "patthana17.19:3.1_3": "aññamaññe tīṇi …pe…",
  "patthana17.19:3.1_4": "vipāke tīṇi …pe…",
  "patthana17.19:3.1_5": "avigate nava.",
  "patthana17.19:4.1_0": "Kusalaṁ bahiddhārammaṇaṁ dhammaṁ paṭicca nakusalo nabahiddhārammaṇo dhammo uppajjati hetupaccayā.",
  "patthana17.19:4.1_1": "(Saṅkhittaṁ.) Hetuyā nava,",
  "patthana17.19:4.1_2": "adhipatiyā nava …pe…",
  "patthana17.19:4.1_3": "aññamaññe tīṇi …pe…",
  "patthana17.19:4.1_4": "vipāke tīṇi …pe…",
  "patthana17.19:4.1_5": "avigate nava."
}