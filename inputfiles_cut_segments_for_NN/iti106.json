{
  "iti106:0.1_0": "Itivuttaka 106",
  "iti106:0.2_0": "Catukkanipāta",
  "iti106:0.3_0": "Brāhmaṇadhammayāgavagga",
  "iti106:0.4_0": "Sabrahmakasutta",
  "iti106:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti106:2.1_0": "“Sabrahmakāni, bhikkhave,",
  "iti106:2.1_1": "tāni kulāni yesaṁ puttānaṁ mātāpitaro ajjhāgāre pūjitā honti.",
  "iti106:2.2_0": "Sapubbadevatāni, bhikkhave,",
  "iti106:2.2_1": "tāni kulāni yesaṁ puttānaṁ mātāpitaro ajjhāgāre pūjitā honti.",
  "iti106:2.3_0": "Sapubbācariyakāni, bhikkhave,",
  "iti106:2.3_1": "tāni kulāni yesaṁ puttānaṁ mātāpitaro ajjhāgāre pūjitā honti.",
  "iti106:2.4_0": "Sāhuneyyakāni, bhikkhave,",
  "iti106:2.4_1": "tāni kulāni yesaṁ puttānaṁ mātāpitaro ajjhāgāre pūjitā honti.",
  "iti106:3.1_0": "‘Brahmā’ti, bhikkhave, mātāpitūnaṁ etaṁ adhivacanaṁ.",
  "iti106:3.2_0": "‘Pubbadevatā’ti, bhikkhave, mātāpitūnaṁ etaṁ adhivacanaṁ.",
  "iti106:3.3_0": "‘Pubbācariyā’ti, bhikkhave, mātāpitūnaṁ etaṁ adhivacanaṁ.",
  "iti106:3.4_0": "‘Āhuneyyā’ti, bhikkhave, mātāpitūnaṁ etaṁ adhivacanaṁ.",
  "iti106:3.5_0": "Taṁ kissa hetu?",
  "iti106:3.6_0": "Bahukārā, bhikkhave,",
  "iti106:3.6_1": "mātāpitaro puttānaṁ āpādakā posakā imassa lokassa dassetāro”ti.",
  "iti106:3.7_0": "Etamatthaṁ bhagavā avoca.",
  "iti106:3.8_0": "Tatthetaṁ iti vuccati:",
  "iti106:4.1_0": "“Brahmāti mātāpitaro,",
  "iti106:4.2_0": "pubbācariyāti vuccare;",
  "iti106:4.3_0": "Āhuneyyā ca puttānaṁ,",
  "iti106:4.4_0": "pajāya anukampakā.",
  "iti106:5.1_0": "Tasmā hi ne namasseyya,",
  "iti106:5.2_0": "sakkareyya ca paṇḍito;",
  "iti106:5.3_0": "Annena atha pānena,",
  "iti106:5.4_0": "vatthena sayanena ca;",
  "iti106:5.5_0": "Ucchādanena nhāpanena,",
  "iti106:5.6_0": "pādānaṁ dhovanena ca.",
  "iti106:6.1_0": "Tāya naṁ pāricariyāya,",
  "iti106:6.2_0": "mātāpitūsu paṇḍitā;",
  "iti106:6.3_0": "Idheva naṁ pasaṁsanti,",
  "iti106:6.4_0": "pecca sagge pamodatī”ti.",
  "iti106:7.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti106:7.2_0": "Sattamaṁ."
}