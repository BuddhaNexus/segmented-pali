{
  "tha-ap81:0.1_0": "Therāpadāna",
  "tha-ap81:0.2_0": "Nāgasamālavagga",
  "tha-ap81:0.3_0": "9. Ṭhitañjaliyattheraapadāna",
  "tha-ap81:1.1_0": "“Migaluddo pure āsiṁ,",
  "tha-ap81:1.2_0": "Araññe kānane ahaṁ;",
  "tha-ap81:1.3_0": "Tattha addasaṁ sambuddhaṁ,",
  "tha-ap81:1.4_0": "Bāttiṁsavaralakkhaṇaṁ.",
  "tha-ap81:2.1_0": "Tatthāhaṁ añjaliṁ katvā,",
  "tha-ap81:2.2_0": "pakkāmiṁ pācināmukho;",
  "tha-ap81:2.3_0": "Avidūre nisinnassa,",
  "tha-ap81:2.4_0": "niyake paṇṇasanthare.",
  "tha-ap81:3.1_0": "Tato me asanīpāto,",
  "tha-ap81:3.2_0": "matthake nipatī tadā;",
  "tha-ap81:3.3_0": "Sohaṁ maraṇakālamhi,",
  "tha-ap81:3.4_0": "akāsiṁ punarañjaliṁ.",
  "tha-ap81:4.1_0": "Dvenavute ito kappe,",
  "tha-ap81:4.2_0": "añjaliṁ akariṁ tadā;",
  "tha-ap81:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap81:4.4_0": "añjalissa idaṁ phalaṁ.",
  "tha-ap81:5.1_0": "Catupaṇṇāsakappamhi,",
  "tha-ap81:5.2_0": "migaketusanāmako;",
  "tha-ap81:5.3_0": "Sattaratanasampanno,",
  "tha-ap81:5.4_0": "cakkavattī mahabbalo.",
  "tha-ap81:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap81:6.2_0": "…pe…",
  "tha-ap81:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap81:7.1_0": "Itthaṁ sudaṁ āyasmā ṭhitañjaliyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap81:8.1_0": "Ṭhitañjaliyattherassāpadānaṁ navamaṁ."
}