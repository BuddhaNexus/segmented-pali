{
  "tha-ap262:0.1_0": "Therāpadāna",
  "tha-ap262:0.2_0": "Thomakavagga",
  "tha-ap262:0.3_0": "10. Aggapupphiyattheraapadāna",
  "tha-ap262:1.1_0": "“Suvaṇṇavaṇṇaṁ sambuddhaṁ,",
  "tha-ap262:1.2_0": "Nisinnaṁ pabbatantare;",
  "tha-ap262:1.3_0": "Obhāsayantaṁ raṁsena,",
  "tha-ap262:1.4_0": "Sikhinaṁ sikhinaṁ yathā.",
  "tha-ap262:2.1_0": "Aggajaṁ pupphamādāya,",
  "tha-ap262:2.2_0": "upāgacchiṁ naruttamaṁ;",
  "tha-ap262:2.3_0": "Pasannacitto sumano,",
  "tha-ap262:2.4_0": "buddhassa abhiropayiṁ.",
  "tha-ap262:3.1_0": "Ekattiṁse ito kappe,",
  "tha-ap262:3.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap262:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap262:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap262:4.1_0": "Pañcavīsatikappamhi,",
  "tha-ap262:4.2_0": "ahosi amitogato;",
  "tha-ap262:4.3_0": "Sattaratanasampanno,",
  "tha-ap262:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap262:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap262:5.2_0": "…pe…",
  "tha-ap262:5.3_0": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap262:6.1_0": "Itthaṁ sudaṁ āyasmā aggapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap262:7.1_0": "Aggapupphiyattherassāpadānaṁ dasamaṁ.",
  "tha-ap262:8.1_0": "Thomakavaggo chabbīsatimo.",
  "tha-ap262:9.1_0": "Tassuddānaṁ",
  "tha-ap262:10.1_0": "Thomakekāsanacitakaṁ,",
  "tha-ap262:10.2_0": "campako sattapāṭali;",
  "tha-ap262:10.3_0": "Pānadhi mañjarī paṇṇaṁ,",
  "tha-ap262:10.4_0": "kuṭido aggapupphiyo;",
  "tha-ap262:10.5_0": "Gāthāyo gaṇitā cettha,",
  "tha-ap262:10.6_0": "ekatālīsameva cāti."
}