{
  "iti36:0.1_0": "Itivuttaka 36",
  "iti36:0.2_0": "Dukanipāta",
  "iti36:0.3_0": "Paṭhamavagga",
  "iti36:0.4_0": "Dutiyajananakuhanasutta",
  "iti36:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti36:2.1_0": "“Nayidaṁ, bhikkhave,",
  "iti36:2.1_1": "brahmacariyaṁ vussati janakuhanatthaṁ, na janalapanatthaṁ,",
  "iti36:2.1_2": "na lābhasakkārasilokānisaṁsatthaṁ, na ‘iti maṁ jano jānātū’ti.",
  "iti36:2.2_0": "Atha kho idaṁ, bhikkhave,",
  "iti36:2.2_1": "brahmacariyaṁ vussati abhiññatthañceva pariññatthañcā”ti.",
  "iti36:2.3_0": "Etamatthaṁ bhagavā avoca.",
  "iti36:2.4_0": "Tatthetaṁ iti vuccati:",
  "iti36:3.1_0": "“Abhiññatthaṁ pariññatthaṁ,",
  "iti36:3.2_0": "brahmacariyaṁ anītihaṁ;",
  "iti36:3.3_0": "Adesayi so bhagavā,",
  "iti36:3.4_0": "nibbānogadhagāminaṁ.",
  "iti36:4.1_0": "Esa maggo mahattehi,",
  "iti36:4.2_0": "anuyāto mahesibhi;",
  "iti36:4.3_0": "Ye ye taṁ paṭipajjanti,",
  "iti36:4.4_0": "yathā buddhena desitaṁ;",
  "iti36:4.5_0": "Dukkhassantaṁ karissanti,",
  "iti36:4.6_0": "satthusāsanakārino”ti.",
  "iti36:5.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti36:5.2_0": "Navamaṁ."
}