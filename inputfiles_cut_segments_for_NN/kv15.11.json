{
  "kv15.11:0.1_0": "Kathāvatthu",
  "kv15.11:0.2_0": "Mahāpaṇṇāsaka",
  "kv15.11:0.3_0": "Pannarasamavagga",
  "kv15.11:0.4_0": "Kammūpacayakathā",
  "kv15.11:1.1_0": "Aññaṁ kammaṁ añño kammūpacayoti?",
  "kv15.11:1.2_0": "Āmantā.",
  "kv15.11:1.3_0": "Añño phasso, añño phassūpacayo;",
  "kv15.11:1.4_0": "aññā vedanā, añño vedanūpacayo;",
  "kv15.11:1.5_0": "aññā saññā, añño saññūpacayo;",
  "kv15.11:1.6_0": "aññā cetanā, añño cetanūpacayo;",
  "kv15.11:1.7_0": "aññaṁ cittaṁ, añño cittūpacayo;",
  "kv15.11:1.8_0": "aññā saddhā, añño saddhūpacayo;",
  "kv15.11:1.9_0": "aññaṁ vīriyaṁ, añño vīriyūpacayo;",
  "kv15.11:1.10_0": "aññā sati, añño satūpacayo;",
  "kv15.11:1.11_0": "añño samādhi, añño samādhūpacayo;",
  "kv15.11:1.12_0": "aññā paññā, añño paññūpacayo;",
  "kv15.11:1.13_0": "añño rāgo, añño rāgūpacayo …pe…",
  "kv15.11:1.14_0": "aññaṁ anottappaṁ, añño anottappūpacayoti?",
  "kv15.11:1.15_0": "Na hevaṁ vattabbe …pe….",
  "kv15.11:2.1_0": "Aññaṁ kammaṁ, añño kammūpacayoti?",
  "kv15.11:2.2_0": "Āmantā.",
  "kv15.11:2.3_0": "Kammūpacayo kammena sahajātoti?",
  "kv15.11:2.4_0": "Na hevaṁ vattabbe …pe….",
  "kv15.11:3.1_0": "Kammūpacayo kammena sahajātoti?",
  "kv15.11:3.2_0": "Āmantā.",
  "kv15.11:3.3_0": "Kusalena kammena sahajāto kammūpacayo kusaloti, na hevaṁ vattabbe …pe….",
  "kv15.11:4.1_0": "Kusalena kammena sahajāto kammūpacayo kusaloti?",
  "kv15.11:4.2_0": "Āmantā.",
  "kv15.11:4.3_0": "Sukhāya vedanāya sampayuttena kammena sahajāto kammūpacayo sukhāya vedanāya sampayuttoti?",
  "kv15.11:4.4_0": "Na hevaṁ vattabbe …pe…",
  "kv15.11:4.5_0": "dukkhāya vedanāya …pe…",
  "kv15.11:4.6_0": "adukkhamasukhāya vedanāya sampayuttena kammena sahajāto kammūpacayo adukkhamasukhāya vedanāya sampayuttoti?",
  "kv15.11:4.7_0": "Na hevaṁ vattabbe …pe….",
  "kv15.11:5.1_0": "Kammūpacayo kammena sahajātoti?",
  "kv15.11:5.2_0": "Āmantā.",
  "kv15.11:5.3_0": "Akusalena kammena sahajāto kammūpacayo akusaloti?",
  "kv15.11:5.4_0": "Na hevaṁ vattabbe …pe…",
  "kv15.11:6.1_0": "Akusalena kammena sahajāto kammūpacayo akusaloti?",
  "kv15.11:6.2_0": "Āmantā.",
  "kv15.11:6.3_0": "Sukhāya vedanāya sampayuttena kammena sahajāto kammūpacayo sukhāya vedanāya sampayuttoti?",
  "kv15.11:6.4_0": "Na hevaṁ vattabbe …pe…",
  "kv15.11:6.5_0": "dukkhāya vedanāya …pe…",
  "kv15.11:6.6_0": "adukkhamasukhāya vedanāya sampayuttena kammena sahajāto kammūpacayo adukkhamasukhāya vedanāya sampayuttoti?",
  "kv15.11:6.7_0": "Na hevaṁ vattabbe …pe….",
  "kv15.11:7.1_0": "Kammaṁ cittena sahajātaṁ, kammaṁ sārammaṇanti?",
  "kv15.11:7.2_0": "Āmantā.",
  "kv15.11:7.3_0": "Kammūpacayo cittena sahajāto, kammūpacayo sārammaṇoti?",
  "kv15.11:7.4_0": "Na hevaṁ vattabbe …pe…",
  "kv15.11:7.5_0": "kammūpacayo cittena sahajāto, kammūpacayo anārammaṇoti?",
  "kv15.11:7.6_0": "Āmantā.",
  "kv15.11:7.7_0": "Kammaṁ cittena sahajātaṁ, kammaṁ anārammaṇanti?",
  "kv15.11:7.8_0": "Na hevaṁ vattabbe …pe….",
  "kv15.11:8.1_0": "Kammaṁ cittena sahajātaṁ, cittaṁ bhijjamānaṁ kammaṁ bhijjatīti?",
  "kv15.11:8.2_0": "Āmantā.",
  "kv15.11:8.3_0": "Kammūpacayo cittena sahajāto, cittaṁ bhijjamānaṁ kammūpacayo bhijjatīti?",
  "kv15.11:8.4_0": "Na hevaṁ vattabbe …pe….",
  "kv15.11:9.1_0": "Kammūpacayo cittena sahajāto, cittaṁ bhijjamānaṁ kammūpacayo na bhijjatīti?",
  "kv15.11:9.2_0": "Āmantā.",
  "kv15.11:9.3_0": "Kammaṁ cittena sahajātaṁ, cittaṁ bhijjamānaṁ kammaṁ na bhijjatīti?",
  "kv15.11:9.4_0": "Na hevaṁ vattabbe …pe….",
  "kv15.11:10.1_0": "Kammamhi kammūpacayoti?",
  "kv15.11:10.2_0": "Āmantā.",
  "kv15.11:10.3_0": "Taññeva kammaṁ so kammūpacayoti?",
  "kv15.11:10.4_0": "Na hevaṁ vattabbe …pe….",
  "kv15.11:11.1_0": "Kammamhi kammūpacayo, kammūpacayato vipāko nibbattatīti?",
  "kv15.11:11.2_0": "Āmantā.",
  "kv15.11:11.3_0": "Taññeva kammaṁ, so kammūpacayo, so kammavipākoti?",
  "kv15.11:11.4_0": "Na hevaṁ vattabbe …pe….",
  "kv15.11:12.1_0": "Kammamhi kammūpacayo, kammūpacayato vipāko nibbattati, vipāko sārammaṇoti?",
  "kv15.11:12.2_0": "Āmantā.",
  "kv15.11:12.3_0": "Kammūpacayo sārammaṇoti?",
  "kv15.11:12.4_0": "Na hevaṁ vattabbe …pe…",
  "kv15.11:12.5_0": "kammūpacayo anārammaṇoti?",
  "kv15.11:12.6_0": "Āmantā.",
  "kv15.11:12.7_0": "Vipāko anārammaṇoti?",
  "kv15.11:12.8_0": "Na hevaṁ vattabbe …pe….",
  "kv15.11:13.1_0": "Aññaṁ kammaṁ añño kammūpacayoti?",
  "kv15.11:13.2_0": "Āmantā.",
  "kv15.11:13.3_0": "Nanu vuttaṁ bhagavatā—",
  "kv15.11:13.4_0": "“idha, puṇṇa,",
  "kv15.11:13.4_1": "ekacco sabyābajjhampi abyābajjhampi kāyasaṅkhāraṁ abhisaṅkharoti,",
  "kv15.11:13.4_2": "sabyābajjhampi abyābajjhampi vacīsaṅkhāraṁ …pe…",
  "kv15.11:13.5_0": "manosaṅkhāraṁ abhisaṅkharoti,",
  "kv15.11:13.5_1": "so sabyābajjhampi abyābajjhampi kāyasaṅkhāraṁ abhisaṅkharitvā,",
  "kv15.11:13.5_2": "sabyābajjhampi abyābajjhampi vacīsaṅkhāraṁ …pe…",
  "kv15.11:13.6_0": "manosaṅkhāraṁ abhisaṅkharitvā sabyābajjhampi abyābajjhampi lokaṁ upapajjati.",
  "kv15.11:13.7_0": "Tamenaṁ sabyābajjhampi abyābajjhampi lokaṁ upapannaṁ samānaṁ sabyābajjhāpi abyābajjhāpi phassā phusanti.",
  "kv15.11:13.8_0": "So sabyābajjhehipi abyābajjhehipi phassehi phuṭṭho samāno sabyābajjhampi abyābajjhampi vedanaṁ vedeti vokiṇṇasukhadukkhaṁ,",
  "kv15.11:13.8_1": "seyyathāpi manussā ekacce ca devā ekacce ca vinipātikā.",
  "kv15.11:13.9_0": "Iti kho, puṇṇa,",
  "kv15.11:13.9_1": "bhūtā bhūtassa upapatti hoti,",
  "kv15.11:13.9_2": "yaṁ karoti tena upapajjati,",
  "kv15.11:13.9_3": "upapannametaṁ phassā phusanti.",
  "kv15.11:13.10_0": "Evampāhaṁ, puṇṇa, ‘kammadāyādā sattā’ti vadāmī”ti.",
  "kv15.11:13.11_0": "Attheva suttantoti?",
  "kv15.11:13.12_0": "Āmantā.",
  "kv15.11:13.13_0": "Tena hi na vattabbaṁ—",
  "kv15.11:13.14_0": "“aññaṁ kammaṁ, añño kammūpacayo”ti.",
  "kv15.11:13.15_0": "Kammūpacayakathā niṭṭhitā.",
  "kv15.11:13.16_0": "Pannarasamo vaggo.",
  "kv15.11:13.17_0": "Tassuddānaṁ",
  "kv15.11:14.1_0": "Paccayatā vavatthitā,",
  "kv15.11:14.2_0": "paṭiccasamuppādo,",
  "kv15.11:14.3_0": "addhā,",
  "kv15.11:14.4_0": "khaṇo layo muhuttaṁ,",
  "kv15.11:14.5_0": "cattāro āsavā anāsavā,",
  "kv15.11:14.6_0": "lokuttarānaṁ dhammānaṁ jarāmaraṇaṁ lokuttarā,",
  "kv15.11:14.7_0": "saññāvedayitanirodhasamāpatti lokuttarā,",
  "kv15.11:14.8_0": "saññāvedayitanirodhasamāpatti lokiyā,",
  "kv15.11:14.9_0": "saññāvedayitanirodhaṁ samāpanno kālaṁ kareyya,",
  "kv15.11:14.10_0": "sveva maggo asaññasattupapattiyā,",
  "kv15.11:14.11_0": "aññaṁ kammaṁ añño kammūpacayoti.",
  "kv15.11:14.12_0": "Tatiyo paṇṇāsako.",
  "kv15.11:14.13_0": "Tassuddānaṁ",
  "kv15.11:15.1_0": "Anusayā,",
  "kv15.11:15.2_0": "saṁvaro,",
  "kv15.11:15.3_0": "kappo,",
  "kv15.11:15.4_0": "mūlañca vavatthitāti."
}