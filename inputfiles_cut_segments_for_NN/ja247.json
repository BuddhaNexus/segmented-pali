{
  "ja247:0.1_0": "Jātaka",
  "ja247:0.2_0": "Dukanipāta",
  "ja247:0.3_0": "Siṅgālavagga",
  "ja247:0.4_0": "7. Pādañjalījātaka",
  "ja247:1.1_0": "“Addhā pādañjalī sabbe,",
  "ja247:1.2_0": "paññāya atirocati;",
  "ja247:1.3_0": "Tathā hi oṭṭhaṁ bhañjati,",
  "ja247:1.4_0": "uttariṁ nūna passati”.",
  "ja247:2.1_0": "“Nāyaṁ dhammaṁ adhammaṁ vā,",
  "ja247:2.2_0": "Atthānatthañca bujjhati;",
  "ja247:2.3_0": "Aññatra oṭṭhanibbhogā,",
  "ja247:2.4_0": "Nāyaṁ jānāti kiñcanan”ti.",
  "ja247:3.1_0": "Pādañjalījātakaṁ sattamaṁ."
}