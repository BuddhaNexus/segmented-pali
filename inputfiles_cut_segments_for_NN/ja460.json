{
  "ja460:0.1_0": "Jātaka",
  "ja460:0.2_0": "Ekādasakanipāta",
  "ja460:0.3_0": "Mātuposakavagga",
  "ja460:0.4_0": "6. Yudhañcayajātaka",
  "ja460:1.1_0": "“Mittāmaccaparibyūḷhaṁ,",
  "ja460:1.2_0": "ahaṁ vande rathesabhaṁ;",
  "ja460:1.3_0": "Pabbajissāmahaṁ rāja,",
  "ja460:1.4_0": "taṁ devo anumaññatu”.",
  "ja460:2.1_0": "“Sace te ūnaṁ kāmehi,",
  "ja460:2.2_0": "ahaṁ paripūrayāmi te;",
  "ja460:2.3_0": "Yo taṁ hiṁsati vāremi,",
  "ja460:2.4_0": "mā pabbaja yudhañcaya”.",
  "ja460:3.1_0": "“Na matthi ūnaṁ kāmehi,",
  "ja460:3.2_0": "hiṁsitā me na vijjati;",
  "ja460:3.3_0": "Dīpañca kātumicchāmi,",
  "ja460:3.4_0": "yaṁ jarā nābhikīrati”.",
  "ja460:4.1_0": "“Putto vā pitaraṁ yāce,",
  "ja460:4.2_0": "pitā vā puttamorasaṁ;",
  "ja460:4.3_0": "Negamo taṁ yāce tāta,",
  "ja460:4.4_0": "mā pabbaja yudhañcaya”.",
  "ja460:5.1_0": "“Mā maṁ deva nivārehi,",
  "ja460:5.2_0": "pabbajantaṁ rathesabha;",
  "ja460:5.3_0": "Māhaṁ kāmehi sammatto,",
  "ja460:5.4_0": "jarāya vasamanvagū”.",
  "ja460:6.1_0": "“Ahaṁ taṁ tāta yācāmi,",
  "ja460:6.2_0": "ahaṁ putta nivāraye;",
  "ja460:6.3_0": "Ciraṁ taṁ daṭṭhumicchāmi,",
  "ja460:6.4_0": "mā pabbaja yudhañcaya”.",
  "ja460:7.1_0": "“Ussāvova tiṇaggamhi,",
  "ja460:7.2_0": "sūriyuggamanaṁ pati;",
  "ja460:7.3_0": "Evamāyu manussānaṁ,",
  "ja460:7.4_0": "mā maṁ amma nivāraya”.",
  "ja460:8.1_0": "“Taramāno imaṁ yānaṁ,",
  "ja460:8.2_0": "āropetu rathesabha;",
  "ja460:8.3_0": "Mā me mātā tarantassa,",
  "ja460:8.4_0": "antarāyakarā ahu”.",
  "ja460:9.1_0": "“Abhidhāvatha bhaddante,",
  "ja460:9.2_0": "suññaṁ hessati rammakaṁ;",
  "ja460:9.3_0": "Yudhañcayo anuññāto,",
  "ja460:9.4_0": "sabbadattena rājinā.",
  "ja460:10.1_0": "Yohu seṭṭho sahassassa,",
  "ja460:10.2_0": "yuvā kañcanasannibho;",
  "ja460:10.3_0": "Soyaṁ kumāro pabbajito,",
  "ja460:10.4_0": "kāsāyavasano balī”.",
  "ja460:11.1_0": "“Ubho kumārā pabbajitā,",
  "ja460:11.2_0": "yudhañcayo yudhiṭṭhilo;",
  "ja460:11.3_0": "Pahāya mātāpitaro,",
  "ja460:11.4_0": "saṅgaṁ chetvāna maccuno”ti.",
  "ja460:12.1_0": "Yudhañcayajātakaṁ chaṭṭhaṁ."
}