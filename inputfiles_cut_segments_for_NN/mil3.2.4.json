{
  "mil3.2.4:0.1_0": "Milindapañha",
  "mil3.2.4:0.2_0": "Addhānavagga",
  "mil3.2.4:0.3_0": "4. Paṭisandahanapuggalavediyanapañha",
  "mil3.2.4:1.1_0": "Rājā āha—",
  "mil3.2.4:1.2_0": "“bhante nāgasena, yo na paṭisandahati,",
  "mil3.2.4:1.2_1": "vedeti so kiñci dukkhaṁ vedanan”ti?",
  "mil3.2.4:1.3_0": "Thero āha—",
  "mil3.2.4:1.4_0": "“kiñci vedeti, kiñci na vedetī”ti.",
  "mil3.2.4:1.5_0": "“Kiṁ vedeti, kiṁ na vedetī”ti?",
  "mil3.2.4:1.6_0": "“Kāyikaṁ, mahārāja, vedanaṁ vedeti,",
  "mil3.2.4:1.6_1": "cetasikaṁ vedanaṁ na vedetī”ti.",
  "mil3.2.4:1.7_0": "“Kathaṁ, bhante, kāyikaṁ vedanaṁ vedeti,",
  "mil3.2.4:1.7_1": "kathaṁ cetasikaṁ vedanaṁ na vedetī”ti?",
  "mil3.2.4:1.8_0": "“Yo hetu yo paccayo kāyikāya dukkhavedanāya uppattiyā,",
  "mil3.2.4:1.8_1": "tassa hetussa tassa paccayassa anuparamā kāyikaṁ dukkhavedanaṁ vedeti,",
  "mil3.2.4:1.8_2": "yo hetu yo paccayo cetasikāya dukkhavedanāya uppattiyā,",
  "mil3.2.4:1.8_3": "tassa hetussa tassa paccayassa uparamā cetasikaṁ dukkhavedanaṁ na vedeti.",
  "mil3.2.4:1.9_0": "Bhāsitampetaṁ, mahārāja, bhagavatā—",
  "mil3.2.4:1.10_0": "‘so ekaṁ vedanaṁ vedeti kāyikaṁ na cetasikan’”ti.",
  "mil3.2.4:2.1_0": "“Bhante nāgasena, yo dukkhaṁ vedanaṁ vedeti, kasmā so na parinibbāyatī”ti?",
  "mil3.2.4:2.2_0": "“Natthi, mahārāja,",
  "mil3.2.4:2.2_1": "arahato anunayo vā paṭigho vā,",
  "mil3.2.4:2.2_2": "na ca arahanto apakkaṁ pātenti paripākaṁ āgamenti paṇḍitā.",
  "mil3.2.4:2.3_0": "Bhāsitampetaṁ, mahārāja,",
  "mil3.2.4:2.3_1": "therena sāriputtena dhammasenāpatinā—",
  "mil3.2.4:3.1_0": "‘Nābhinandāmi maraṇaṁ,",
  "mil3.2.4:3.2_0": "nābhinandāmi jīvitaṁ;",
  "mil3.2.4:3.3_0": "Kālañca paṭikaṅkhāmi,",
  "mil3.2.4:3.4_0": "nibbisaṁ bhatako yathā.",
  "mil3.2.4:4.1_0": "Nābhinandāmi maraṇaṁ,",
  "mil3.2.4:4.2_0": "nābhinandāmi jīvitaṁ;",
  "mil3.2.4:4.3_0": "Kālañca paṭikaṅkhāmi,",
  "mil3.2.4:4.4_0": "sampajāno patissato’”ti.",
  "mil3.2.4:5.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.2.4:6.1_0": "Paṭisandahanapuggalavediyanapañho catuttho."
}