{
  "thag1.106:0.1_0": "Theragāthā",
  "thag1.106:0.2_0": "Ekakanipāta",
  "thag1.106:0.3_0": "Ekādasamavagga",
  "thag1.106:0.4_0": "1.106. Suhemantattheragāthā",
  "thag1.106:1.1_0": "“Sataliṅgassa atthassa,",
  "thag1.106:1.2_0": "satalakkhaṇadhārino;",
  "thag1.106:1.3_0": "Ekaṅgadassī dummedho,",
  "thag1.106:1.4_0": "satadassī ca paṇḍito”ti.",
  "thag1.106:1.5_0": "… Suhemanto thero …."
}