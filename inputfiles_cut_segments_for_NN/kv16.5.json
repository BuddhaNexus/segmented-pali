{
  "kv16.5:0.1_0": "Kathāvatthu",
  "kv16.5:0.2_0": "Mahāpaṇṇāsaka",
  "kv16.5:0.3_0": "Soḷasamavagga",
  "kv16.5:0.4_0": "Rūpaṁhetūtikathā",
  "kv16.5:1.1_0": "Rūpaṁ hetūti?",
  "kv16.5:1.2_0": "Āmantā.",
  "kv16.5:1.3_0": "Alobho hetūti?",
  "kv16.5:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv16.5:1.5_0": "adoso hetu …pe…",
  "kv16.5:1.6_0": "amoho hetu …",
  "kv16.5:1.7_0": "lobho hetu …",
  "kv16.5:1.8_0": "doso hetu …",
  "kv16.5:1.9_0": "moho hetūti?",
  "kv16.5:1.10_0": "Na hevaṁ vattabbe …pe….",
  "kv16.5:2.1_0": "Rūpaṁ hetūti?",
  "kv16.5:2.2_0": "Āmantā.",
  "kv16.5:2.3_0": "Sārammaṇaṁ, atthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv16.5:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv16.5:2.5_0": "nanu anārammaṇaṁ,",
  "kv16.5:2.5_1": "natthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv16.5:2.6_0": "Āmantā.",
  "kv16.5:2.7_0": "Hañci anārammaṇaṁ,",
  "kv16.5:2.7_1": "natthi tassa āvaṭṭanā …pe… paṇidhi, no ca vata re vattabbe—",
  "kv16.5:2.8_0": "“rūpaṁ hetū”ti.",
  "kv16.5:3.1_0": "Alobho hetu sārammaṇo,",
  "kv16.5:3.1_1": "atthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv16.5:3.2_0": "Āmantā.",
  "kv16.5:3.3_0": "Rūpaṁ hetu sārammaṇaṁ,",
  "kv16.5:3.3_1": "atthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv16.5:3.4_0": "Na hevaṁ vattabbe …pe…",
  "kv16.5:3.5_0": "adoso hetu …",
  "kv16.5:3.6_0": "amoho hetu …",
  "kv16.5:3.7_0": "lobho hetu …",
  "kv16.5:3.8_0": "doso hetu …",
  "kv16.5:3.9_0": "moho hetu sārammaṇo,",
  "kv16.5:3.9_1": "atthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv16.5:3.10_0": "Āmantā.",
  "kv16.5:3.11_0": "Rūpaṁ hetu sārammaṇaṁ,",
  "kv16.5:3.11_1": "atthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv16.5:3.12_0": "Na hevaṁ vattabbe …pe….",
  "kv16.5:4.1_0": "Rūpaṁ hetu anārammaṇaṁ,",
  "kv16.5:4.1_1": "natthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv16.5:4.2_0": "Āmantā.",
  "kv16.5:4.3_0": "Alobho hetu anārammaṇo,",
  "kv16.5:4.3_1": "natthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv16.5:4.4_0": "Na hevaṁ vattabbe …pe…",
  "kv16.5:4.5_0": "rūpaṁ hetu anārammaṇaṁ,",
  "kv16.5:4.5_1": "natthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv16.5:4.6_0": "Āmantā.",
  "kv16.5:4.7_0": "Adoso hetu …",
  "kv16.5:4.8_0": "amoho hetu …",
  "kv16.5:4.9_0": "lobho hetu …",
  "kv16.5:4.10_0": "doso hetu …",
  "kv16.5:4.11_0": "moho hetu anārammaṇo,",
  "kv16.5:4.11_1": "natthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv16.5:4.12_0": "Na hevaṁ vattabbe …pe….",
  "kv16.5:5.1_0": "Na vattabbaṁ—",
  "kv16.5:5.2_0": "“rūpaṁ hetū”ti?",
  "kv16.5:5.3_0": "Āmantā.",
  "kv16.5:5.4_0": "Nanu mahābhūtā upādāyarūpānaṁ upādāyahetūti?",
  "kv16.5:5.5_0": "Āmantā.",
  "kv16.5:5.6_0": "Hañci mahābhūtā upādāyarūpānaṁ upādāyahetu, tena vata re vattabbe—",
  "kv16.5:5.7_0": "“rūpaṁ hetū”ti.",
  "kv16.5:5.8_0": "Rūpaṁ hetūtikathā niṭṭhitā."
}