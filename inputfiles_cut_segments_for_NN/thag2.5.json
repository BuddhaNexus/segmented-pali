{
  "thag2.5:0.1_0": "Theragāthā",
  "thag2.5:0.2_0": "Dukanipāta",
  "thag2.5:0.3_0": "Paṭhamavagga",
  "thag2.5:0.4_0": "2.5. Ajinattheragāthā",
  "thag2.5:1.1_0": "“Api ce hoti tevijjo,",
  "thag2.5:1.2_0": "maccuhāyī anāsavo;",
  "thag2.5:1.3_0": "Appaññātoti naṁ bālā,",
  "thag2.5:1.4_0": "avajānanti ajānatā.",
  "thag2.5:2.1_0": "Yo ca kho annapānassa,",
  "thag2.5:2.2_0": "lābhī hotidha puggalo;",
  "thag2.5:2.3_0": "Pāpadhammopi ce hoti,",
  "thag2.5:2.4_0": "so nesaṁ hoti sakkato”ti.",
  "thag2.5:2.5_0": "… Ajino thero …."
}