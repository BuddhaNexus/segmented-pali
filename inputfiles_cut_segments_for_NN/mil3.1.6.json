{
  "mil3.1.6:0.1_0": "Milindapañha",
  "mil3.1.6:0.2_0": "Mahāvagga",
  "mil3.1.6:0.3_0": "6. Paṭisandhipañha",
  "mil3.1.6:1.1_0": "Rājā āha—",
  "mil3.1.6:1.2_0": "“bhante nāgasena,",
  "mil3.1.6:1.2_1": "atthi koci mato na paṭisandahatī”ti.",
  "mil3.1.6:1.3_0": "Thero āha—",
  "mil3.1.6:1.4_0": "“koci paṭisandahati, koci na paṭisandahatī”ti.",
  "mil3.1.6:1.5_0": "“Ko paṭisandahati, ko na paṭisandahatī”ti?",
  "mil3.1.6:1.6_0": "“Sakileso, mahārāja, paṭisandahati,",
  "mil3.1.6:1.6_1": "nikkileso na paṭisandahatī”ti.",
  "mil3.1.6:1.7_0": "“Tvaṁ pana, bhante nāgasena, paṭisandahissasī”ti?",
  "mil3.1.6:1.8_0": "“Sace, mahārāja,",
  "mil3.1.6:1.8_1": "saupādāno bhavissāmi paṭisandahissāmi,",
  "mil3.1.6:1.8_2": "sace anupādāno bhavissāmi na paṭisandahissāmī”ti.",
  "mil3.1.6:2.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.1.6:3.1_0": "Paṭisandhipañho chaṭṭho."
}