{
  "pli-tv-bu-vb-sk26:0.1_0": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk26:0.2_0": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk26:0.3_0": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk26:0.4_0": "Khambhakatavagga",
  "pli-tv-bu-vb-sk26:0.5_0": "26. Pallatthikasikkhāpada",
  "pli-tv-bu-vb-sk26:1.1_0": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk26:1.2_0": "Tena kho pana samayena chabbaggiyā bhikkhū pallatthikāya antaraghare nisīdanti …pe….",
  "pli-tv-bu-vb-sk26:1.3.1_0": "“Na pallatthikāya antaraghare nisīdissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk26:1.4_0": "Na pallatthikāya antaraghare nisīditabbaṁ.",
  "pli-tv-bu-vb-sk26:1.5_0": "Yo anādariyaṁ paṭicca hatthapallatthikāya vā dussapallatthikāya vā antaraghare nisīdati,",
  "pli-tv-bu-vb-sk26:1.5_1": "āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk26:1.6.1_0": "Anāpatti—",
  "pli-tv-bu-vb-sk26:1.7_0": "asañcicca,",
  "pli-tv-bu-vb-sk26:1.8_0": "assatiyā,",
  "pli-tv-bu-vb-sk26:1.9_0": "ajānantassa,",
  "pli-tv-bu-vb-sk26:1.10_0": "gilānassa,",
  "pli-tv-bu-vb-sk26:1.11_0": "vāsūpagatassa,",
  "pli-tv-bu-vb-sk26:1.12_0": "āpadāsu,",
  "pli-tv-bu-vb-sk26:1.13_0": "ummattakassa,",
  "pli-tv-bu-vb-sk26:1.14_0": "ādikammikassāti.",
  "pli-tv-bu-vb-sk26:1.15_0": "Chaṭṭhasikkhāpadaṁ niṭṭhitaṁ."
}