{
  "vv29:0.1_0": "Vimānavatthu",
  "vv29:0.2_0": "Itthivimāna",
  "vv29:0.3_0": "Pāricchattakavagga",
  "vv29:0.4_0": "1. Uḷāravimānavatthu",
  "vv29:1.1_0": "“Uḷāro te yaso vaṇṇo,",
  "vv29:1.2_0": "sabbā obhāsate disā;",
  "vv29:1.3_0": "Nāriyo naccanti gāyanti,",
  "vv29:1.4_0": "devaputtā alaṅkatā.",
  "vv29:2.1_0": "Modenti parivārenti,",
  "vv29:2.2_0": "tava pūjāya devate;",
  "vv29:2.3_0": "Sovaṇṇāni vimānāni,",
  "vv29:2.4_0": "tavimāni sudassane.",
  "vv29:3.1_0": "Tuvaṁsi issarā tesaṁ,",
  "vv29:3.2_0": "sabbakāmasamiddhinī;",
  "vv29:3.3_0": "Abhijātā mahantāsi,",
  "vv29:3.4_0": "devakāye pamodasi;",
  "vv29:3.5_0": "Devate pucchitācikkha,",
  "vv29:3.6_0": "kissa kammassidaṁ phalan”ti.",
  "vv29:4.1_0": "“Ahaṁ manussesu manussabhūtā,",
  "vv29:4.2_0": "Purimāya jātiyā manussaloke;",
  "vv29:4.3_0": "Dussīlakule suṇisā ahosiṁ,",
  "vv29:4.4_0": "Assaddhesu kadariyesu ahaṁ.",
  "vv29:5.1_0": "Saddhā sīlena sampannā,",
  "vv29:5.2_0": "saṁvibhāgaratā sadā;",
  "vv29:5.3_0": "Piṇḍāya caramānassa,",
  "vv29:5.4_0": "apūvaṁ te adāsahaṁ.",
  "vv29:6.1_0": "Tadāhaṁ sassuyācikkhiṁ,",
  "vv29:6.2_0": "samaṇo āgato idha;",
  "vv29:6.3_0": "Tassa adāsahaṁ pūvaṁ,",
  "vv29:6.4_0": "pasannā sehi pāṇibhi.",
  "vv29:7.1_0": "Itissā sassu paribhāsi,",
  "vv29:7.2_0": "avinītāsi tvaṁ vadhu;",
  "vv29:7.3_0": "Na maṁ sampucchituṁ icchi,",
  "vv29:7.4_0": "samaṇassa dadāmahaṁ.",
  "vv29:8.1_0": "Tato me sassu kupitā,",
  "vv29:8.2_0": "pahāsi musalena maṁ;",
  "vv29:8.3_0": "Kūṭaṅgacchi avadhi maṁ,",
  "vv29:8.4_0": "nāsakkhiṁ jīvituṁ ciraṁ.",
  "vv29:9.1_0": "Sā ahaṁ kāyassa bhedā,",
  "vv29:9.2_0": "vippamuttā tato cutā;",
  "vv29:9.3_0": "Devānaṁ tāvatiṁsānaṁ,",
  "vv29:9.4_0": "upapannā sahabyataṁ.",
  "vv29:10.1_0": "Tena metādiso vaṇṇo,",
  "vv29:10.2_0": "…pe…",
  "vv29:10.3_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv29:11.1_0": "Uḷāravimānaṁ paṭhamaṁ."
}