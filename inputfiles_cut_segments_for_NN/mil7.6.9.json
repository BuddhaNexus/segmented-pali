{
  "mil7.6.9:0.1_0": "Milindapañha",
  "mil7.6.9:0.2_0": "Opammakathāpañha",
  "mil7.6.9:0.3_0": "Sīhavagga",
  "mil7.6.9:0.4_0": "9. Sappaṅgapañha",
  "mil7.6.9:1.1_0": "“Bhante nāgasena,",
  "mil7.6.9:1.1_1": "‘sappassa tīṇi aṅgāni gahetabbānī’ti yaṁ vadesi,",
  "mil7.6.9:1.1_2": "katamāni tāni tīṇi aṅgāni gahetabbānī”ti?",
  "mil7.6.9:1.2_0": "“Yathā, mahārāja, sappo urena gacchati;",
  "mil7.6.9:1.3_0": "evameva kho, mahārāja,",
  "mil7.6.9:1.3_1": "yoginā yogāvacarena paññāya caritabbaṁ, paññāya caramānassa kho, mahārāja, yogino cittaṁ ñāye carati, vilakkhaṇaṁ vivajjeti, salakkhaṇaṁ bhāveti.",
  "mil7.6.9:1.4_0": "Idaṁ, mahārāja,",
  "mil7.6.9:1.4_1": "sappassa paṭhamaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.6.9:2.1_0": "Puna caparaṁ, mahārāja,",
  "mil7.6.9:2.1_1": "sappo caramāno osadhaṁ parivajjento carati;",
  "mil7.6.9:2.2_0": "evameva kho, mahārāja,",
  "mil7.6.9:2.2_1": "yoginā yogāvacarena duccaritaṁ parivajjentena caritabbaṁ.",
  "mil7.6.9:2.3_0": "Idaṁ, mahārāja, sappassa dutiyaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.6.9:3.1_0": "Puna caparaṁ, mahārāja,",
  "mil7.6.9:3.1_1": "sappo manusse disvā tappati socati cintayati;",
  "mil7.6.9:3.2_0": "evameva kho, mahārāja,",
  "mil7.6.9:3.2_1": "yoginā yogāvacarena kuvitakke,",
  "mil7.6.9:3.2_2": "vitakketvā aratiṁ uppādayitvā tappitabbaṁ socitabbaṁ cintayitabbaṁ ‘pamādena me divaso vītināmito, na so puna sakkā laddhun’ti.",
  "mil7.6.9:3.3_0": "Idaṁ, mahārāja, sappassa tatiyaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.6.9:3.4_0": "Bhāsitampetaṁ, mahārāja,",
  "mil7.6.9:3.4_1": "bhagavatā bhallāṭiyajātake dvinnaṁ kinnarānaṁ—",
  "mil7.6.9:4.1_0": "‘Mayekarattaṁ vippavasimha ludda,",
  "mil7.6.9:4.2_0": "Akāmakā aññamaññaṁ sarantā;",
  "mil7.6.9:4.3_0": "Tamekarattaṁ anutappamānā,",
  "mil7.6.9:4.4_0": "Socāmasā ratti puna na hessatī’”ti.",
  "mil7.6.9:5.1_0": "Sappaṅgapañho navamo."
}