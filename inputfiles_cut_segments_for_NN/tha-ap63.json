{
  "tha-ap63:0.1_0": "Therāpadāna",
  "tha-ap63:0.2_0": "Sakacintaniyavagga",
  "tha-ap63:0.3_0": "1 Sakacintaniyattheraapadāna",
  "tha-ap63:1.1_0": "“Pavanaṁ kānanaṁ disvā,",
  "tha-ap63:1.2_0": "appasaddamanāvilaṁ;",
  "tha-ap63:1.3_0": "Isīnaṁ anuciṇṇaṁva,",
  "tha-ap63:1.4_0": "āhutīnaṁ paṭiggahaṁ.",
  "tha-ap63:2.1_0": "Thūpaṁ katvāna pulinaṁ,",
  "tha-ap63:2.2_0": "nānāpupphaṁ samokiriṁ;",
  "tha-ap63:2.3_0": "Sammukhā viya sambuddhaṁ,",
  "tha-ap63:2.4_0": "nimmitaṁ abhivandahaṁ.",
  "tha-ap63:3.1_0": "Sattaratanasampanno,",
  "tha-ap63:3.2_0": "rājā raṭṭhamhi issaro;",
  "tha-ap63:3.3_0": "Sakakammābhiraddhohaṁ,",
  "tha-ap63:3.4_0": "pupphapūjāyidaṁ phalaṁ.",
  "tha-ap63:4.1_0": "Ekanavutito kappe,",
  "tha-ap63:4.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap63:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap63:4.4_0": "pupphapūjāyidaṁ phalaṁ.",
  "tha-ap63:5.1_0": "Asītikappenantayaso,",
  "tha-ap63:5.2_0": "cakkavattī ahosahaṁ;",
  "tha-ap63:5.3_0": "Sattaratanasampanno,",
  "tha-ap63:5.4_0": "catudīpamhi issaro.",
  "tha-ap63:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap63:6.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap63:6.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap63:6.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap63:7.1_0": "Itthaṁ sudaṁ āyasmā sakacintaniyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap63:8.1_0": "Sakacintaniyattherassāpadānaṁ paṭhamaṁ."
}