{
  "ja120:0.1_0": "Jātaka",
  "ja120:0.2_0": "Ekakanipāta",
  "ja120:0.3_0": "Haṁcivagga",
  "ja120:0.4_0": "10. Bandhanamokkhajātaka",
  "ja120:1.1_0": "“Abaddhā tattha bajjhanti,",
  "ja120:1.2_0": "yattha bālā pabhāsare;",
  "ja120:1.3_0": "Baddhāpi tattha muccanti,",
  "ja120:1.4_0": "yattha dhīrā pabhāsare”ti.",
  "ja120:2.1_0": "Bandhanamokkhajātakaṁ dasamaṁ.",
  "ja120:2.2_0": "Haṁcivaggo dvādasamo.",
  "ja120:3.1_0": "Tassuddānaṁ",
  "ja120:4.1_0": "Atha gadrabha sattuva kaṁsasataṁ,",
  "ja120:4.2_0": "Bahucinti sāsikāyātikara;",
  "ja120:4.3_0": "Ativela visesamanācariyova,",
  "ja120:4.4_0": "Dhīrāpabhāsaratena dasāti."
}