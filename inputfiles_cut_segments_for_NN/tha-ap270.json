{
  "tha-ap270:0.1_0": "Therāpadāna",
  "tha-ap270:0.2_0": "Padumukkhipavagga",
  "tha-ap270:0.3_0": "8 Saḷalapupphiyattheraapadāna",
  "tha-ap270:1.1_0": "“Candabhāgānadītīre,",
  "tha-ap270:1.2_0": "ahosiṁ kinnaro tadā;",
  "tha-ap270:1.3_0": "Vipassiṁ addasaṁ buddhaṁ,",
  "tha-ap270:1.4_0": "raṁsijālasamākulaṁ.",
  "tha-ap270:2.1_0": "Pasannacitto sumano,",
  "tha-ap270:2.2_0": "paramāya ca pītiyā;",
  "tha-ap270:2.3_0": "Paggayha saḷalaṁ pupphaṁ,",
  "tha-ap270:2.4_0": "vipassiṁ okiriṁ ahaṁ.",
  "tha-ap270:3.1_0": "Ekanavutito kappe,",
  "tha-ap270:3.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap270:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap270:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap270:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap270:4.2_0": "…pe…",
  "tha-ap270:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap270:5.1_0": "Itthaṁ sudaṁ āyasmā saḷalapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap270:6.1_0": "Saḷalapupphiyattherassāpadānaṁ aṭṭhamaṁ."
}