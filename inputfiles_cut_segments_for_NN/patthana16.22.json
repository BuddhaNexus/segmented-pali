{
  "patthana16.22:0.1_0": "Paṭṭhānapakaraṇa",
  "patthana16.22:0.2_0": "Dhammānulomapaccanīya (3), Tikadukapaṭṭhānapāḷi (4)",
  "patthana16.22:0.3_0": "16.22. Kusalattika, Cittānuparivattadukādi",
  "patthana16.22:1.1_0": "Kusalaṁ cittānuparivattiṁ dhammaṁ paṭicca …",
  "patthana16.22:1.1_1": "(Saṅkhittaṁ.) Hetuyā terasa,",
  "patthana16.22:1.1_2": "ārammaṇe nava …pe…",
  "patthana16.22:1.1_3": "vipāke tīṇi …pe…",
  "patthana16.22:1.1_4": "avigate terasa.",
  "patthana16.22:2.1_0": "Kusalaṁ nocittānuparivattiṁ dhammaṁ paṭicca …",
  "patthana16.22:2.1_1": "(Saṅkhittaṁ.) Hetuyā terasa,",
  "patthana16.22:2.1_2": "ārammaṇe nava …pe…",
  "patthana16.22:2.1_3": "vipāke tīṇi …pe…",
  "patthana16.22:2.1_4": "avigate terasa.",
  "patthana16.22:2.1_5": "(Ete saṅkhittā,",
  "patthana16.22:2.1_6": "dukattayaṁ cittadukasadisaṁ.)"
