{
  "thag1.20:0.1_0": "Theragāthā",
  "thag1.20:0.2_0": "Ekakanipāta",
  "thag1.20:0.3_0": "Dutiyavagga",
  "thag1.20:0.4_0": "1.20. Ajitattheragāthā",
  "thag1.20:1.1_0": "“Maraṇe me bhayaṁ natthi,",
  "thag1.20:1.2_0": "nikanti natthi jīvite;",
  "thag1.20:1.3_0": "Sandehaṁ nikkhipissāmi,",
  "thag1.20:1.4_0": "sampajāno paṭissato”ti.",
  "thag1.20:1.5_0": "…",
  "thag1.20:1.6_0": "Ajito thero …",
  "thag1.20:1.7_0": "vaggo dutiyo.",
  "thag1.20:2.0_0": "Tassuddānaṁ",
  "thag1.20:2.1_0": "Cūḷavaccho mahāvaccho,",
  "thag1.20:2.2_0": "vanavaccho ca sīvako;",
  "thag1.20:2.3_0": "Kuṇḍadhāno ca belaṭṭhi,",
  "thag1.20:2.4_0": "dāsako ca tatopari;",
  "thag1.20:2.5_0": "Siṅgālapitiko thero,",
  "thag1.20:2.6_0": "kulo ca ajito dasāti."
}