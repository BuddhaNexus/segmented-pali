{
  "vv51:0.1_0": "Vimānavatthu",
  "vv51:0.2_0": "Purisavimāna",
  "vv51:0.3_0": "Mahārathavagga",
  "vv51:0.4_0": "1. Maṇḍūkadevaputtavimānavatthu",
  "vv51:1.1_0": "“Ko me vandati pādāni,",
  "vv51:1.2_0": "iddhiyā yasasā jalaṁ;",
  "vv51:1.3_0": "Abhikkantena vaṇṇena,",
  "vv51:1.4_0": "sabbā obhāsayaṁ disā”ti.",
  "vv51:2.1_0": "“Maṇḍūkohaṁ pure āsiṁ,",
  "vv51:2.2_0": "udake vārigocaro;",
  "vv51:2.3_0": "Tava dhammaṁ suṇantassa,",
  "vv51:2.4_0": "avadhī vacchapālako.",
  "vv51:3.1_0": "Muhuttaṁ cittapasādassa,",
  "vv51:3.2_0": "iddhiṁ passa yasañca me;",
  "vv51:3.3_0": "Ānubhāvañca me passa,",
  "vv51:3.4_0": "vaṇṇaṁ passa jutiñca me.",
  "vv51:4.1_0": "Ye ca te dīghamaddhānaṁ,",
  "vv51:4.2_0": "dhammaṁ assosuṁ gotama;",
  "vv51:4.3_0": "Pattā te acalaṭṭhānaṁ,",
  "vv51:4.4_0": "yattha gantvā na socare”ti.",
  "vv51:5.1_0": "Maṇḍūkadevaputtavimānaṁ paṭhamaṁ."
}