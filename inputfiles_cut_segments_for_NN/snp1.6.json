{
  "snp1.6:0.1_0": "Sutta Nipāta 1.6",
  "snp1.6:0.2_0": "Parābhavasutta",
  "snp1.6:1.1_0": "Evaṁ me sutaṁ—",
  "snp1.6:1.2_0": " ekaṁ samayaṁ bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "snp1.6:1.3_0": "Atha kho aññatarā devatā abhikkantāya rattiyā abhikkantavaṇṇā kevalakappaṁ jetavanaṁ obhāsetvā yena bhagavā tenupasaṅkami;",
  "snp1.6:1.3_1": "upasaṅkamitvā bhagavantaṁ abhivādetvā ekamantaṁ aṭṭhāsi.",
  "snp1.6:1.4_0": "Ekamantaṁ ṭhitā kho sā devatā bhagavantaṁ gāthāya ajjhabhāsi:",
  "snp1.6:2.1_0": "“Parābhavantaṁ purisaṁ,",
  "snp1.6:2.2_0": "mayaṁ pucchāma gotama;",
  "snp1.6:2.3_0": "Bhavantaṁ puṭṭhumāgamma,",
  "snp1.6:2.4_0": "kiṁ parābhavato mukhaṁ”.",
  "snp1.6:3.1_0": "“Suvijāno bhavaṁ hoti,",
  "snp1.6:3.2_0": "suvijāno parābhavo;",
  "snp1.6:3.3_0": "Dhammakāmo bhavaṁ hoti,",
  "snp1.6:3.4_0": "dhammadessī parābhavo”.",
  "snp1.6:4.1_0": "“Iti hetaṁ vijānāma,",
  "snp1.6:4.2_0": "paṭhamo so parābhavo;",
  "snp1.6:4.3_0": "Dutiyaṁ bhagavā brūhi,",
  "snp1.6:4.4_0": "kiṁ parābhavato mukhaṁ”.",
  "snp1.6:5.1_0": "“Asantassa piyā honti,",
  "snp1.6:5.2_0": "sante na kurute piyaṁ;",
  "snp1.6:5.3_0": "Asataṁ dhammaṁ roceti,",
  "snp1.6:5.4_0": "taṁ parābhavato mukhaṁ”.",
  "snp1.6:6.1_0": "“Iti hetaṁ vijānāma,",
  "snp1.6:6.2_0": "dutiyo so parābhavo;",
  "snp1.6:6.3_0": "Tatiyaṁ bhagavā brūhi,",
  "snp1.6:6.4_0": "kiṁ parābhavato mukhaṁ”.",
  "snp1.6:7.1_0": "“Niddāsīlī sabhāsīlī,",
  "snp1.6:7.2_0": "anuṭṭhātā ca yo naro;",
  "snp1.6:7.3_0": "Alaso kodhapaññāṇo,",
  "snp1.6:7.4_0": "taṁ parābhavato mukhaṁ”.",
  "snp1.6:8.1_0": "“Iti hetaṁ vijānāma,",
  "snp1.6:8.2_0": "tatiyo so parābhavo;",
  "snp1.6:8.3_0": "Catutthaṁ bhagavā brūhi,",
  "snp1.6:8.4_0": "kiṁ parābhavato mukhaṁ”.",
  "snp1.6:9.1_0": "“Yo mātaraṁ pitaraṁ vā,",
  "snp1.6:9.2_0": "jiṇṇakaṁ gatayobbanaṁ;",
  "snp1.6:9.3_0": "Pahu santo na bharati,",
  "snp1.6:9.4_0": "taṁ parābhavato mukhaṁ”.",
  "snp1.6:10.1_0": "“Iti hetaṁ vijānāma,",
  "snp1.6:10.2_0": "catuttho so parābhavo;",
  "snp1.6:10.3_0": "Pañcamaṁ bhagavā brūhi,",
  "snp1.6:10.4_0": "kiṁ parābhavato mukhaṁ”.",
  "snp1.6:11.1_0": "“Yo brāhmaṇaṁ samaṇaṁ vā,",
  "snp1.6:11.2_0": "Aññaṁ vāpi vanibbakaṁ;",
  "snp1.6:11.3_0": "Musāvādena vañceti,",
  "snp1.6:11.4_0": "Taṁ parābhavato mukhaṁ”.",
  "snp1.6:12.1_0": "“Iti hetaṁ vijānāma,",
  "snp1.6:12.2_0": "Pañcamo so parābhavo;",
  "snp1.6:12.3_0": "Chaṭṭhamaṁ bhagavā brūhi,",
  "snp1.6:12.4_0": "Kiṁ parābhavato mukhaṁ”.",
  "snp1.6:13.1_0": "“Pahūtavitto puriso,",
  "snp1.6:13.2_0": "Sahirañño sabhojano;",
  "snp1.6:13.3_0": "Eko bhuñjati sādūni,",
  "snp1.6:13.4_0": "Taṁ parābhavato mukhaṁ”.",
  "snp1.6:14.1_0": "“Iti hetaṁ vijānāma,",
  "snp1.6:14.2_0": "Chaṭṭhamo so parābhavo;",
  "snp1.6:14.3_0": "Sattamaṁ bhagavā brūhi,",
  "snp1.6:14.4_0": "Kiṁ parābhavato mukhaṁ”.",
  "snp1.6:15.1_0": "“Jātitthaddho dhanatthaddho,",
  "snp1.6:15.2_0": "Gottatthaddho ca yo naro;",
  "snp1.6:15.3_0": "Saññātiṁ atimaññeti,",
  "snp1.6:15.4_0": "Taṁ parābhavato mukhaṁ”.",
  "snp1.6:16.1_0": "“Iti hetaṁ vijānāma,",
  "snp1.6:16.2_0": "Sattamo so parābhavo;",
  "snp1.6:16.3_0": "Aṭṭhamaṁ bhagavā brūhi,",
  "snp1.6:16.4_0": "Kiṁ parābhavato mukhaṁ”.",
  "snp1.6:17.1_0": "“Itthidhutto surādhutto,",
  "snp1.6:17.2_0": "Akkhadhutto ca yo naro;",
  "snp1.6:17.3_0": "Laddhaṁ laddhaṁ vināseti,",
  "snp1.6:17.4_0": "Taṁ parābhavato mukhaṁ”.",
  "snp1.6:18.1_0": "“Iti hetaṁ vijānāma,",
  "snp1.6:18.2_0": "Aṭṭhamo so parābhavo;",
  "snp1.6:18.3_0": "Navamaṁ bhagavā brūhi,",
  "snp1.6:18.4_0": "Kiṁ parābhavato mukhaṁ”.",
  "snp1.6:19.1_0": "“Sehi dārehi asantuṭṭho,",
  "snp1.6:19.2_0": "Vesiyāsu padussati;",
  "snp1.6:19.3_0": "Dussati paradāresu,",
  "snp1.6:19.4_0": "Taṁ parābhavato mukhaṁ”.",
  "snp1.6:20.1_0": "“Iti hetaṁ vijānāma,",
  "snp1.6:20.2_0": "Navamo so parābhavo;",
  "snp1.6:20.3_0": "Dasamaṁ bhagavā brūhi,",
  "snp1.6:20.4_0": "Kiṁ parābhavato mukhaṁ”.",
  "snp1.6:21.1_0": "“Atītayobbano poso,",
  "snp1.6:21.2_0": "Āneti timbarutthaniṁ;",
  "snp1.6:21.3_0": "Tassā issā na supati,",
  "snp1.6:21.4_0": "Taṁ parābhavato mukhaṁ”.",
  "snp1.6:22.1_0": "“Iti hetaṁ vijānāma,",
  "snp1.6:22.2_0": "Dasamo so parābhavo;",
  "snp1.6:22.3_0": "Ekādasamaṁ bhagavā brūhi,",
  "snp1.6:22.4_0": "Kiṁ parābhavato mukhaṁ”.",
  "snp1.6:23.1_0": "“Itthiṁ soṇḍiṁ vikiraṇiṁ,",
  "snp1.6:23.2_0": "Purisaṁ vāpi tādisaṁ;",
  "snp1.6:23.3_0": "Issariyasmiṁ ṭhapeti,",
  "snp1.6:23.4_0": "Taṁ parābhavato mukhaṁ”.",
  "snp1.6:24.1_0": "“Iti hetaṁ vijānāma,",
  "snp1.6:24.2_0": "Ekādasamo so parābhavo;",
  "snp1.6:24.3_0": "Dvādasamaṁ bhagavā brūhi,",
  "snp1.6:24.4_0": "Kiṁ parābhavato mukhaṁ”.",
  "snp1.6:25.1_0": "“Appabhogo mahātaṇho,",
  "snp1.6:25.2_0": "Khattiye jāyate kule;",
  "snp1.6:25.3_0": "So ca rajjaṁ patthayati,",
  "snp1.6:25.4_0": "Taṁ parābhavato mukhaṁ.",
  "snp1.6:26.1_0": "Ete parābhave loke,",
  "snp1.6:26.2_0": "Paṇḍito samavekkhiya;",
  "snp1.6:26.3_0": "Ariyo dassanasampanno,",
  "snp1.6:26.4_0": "Sa lokaṁ bhajate sivan”ti.",
  "snp1.6:26.5_0": "Parābhavasuttaṁ chaṭṭhaṁ."
}