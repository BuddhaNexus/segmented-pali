{
  "kv8.4:0.1_0": "Kathāvatthu",
  "kv8.4:0.2_0": "Mahāpaṇṇāsaka",
  "kv8.4:0.3_0": "Aṭṭhamavagga",
  "kv8.4:0.4_0": "Kāmakathā",
  "kv8.4:1.1_0": "Pañcevāyatanā kāmāti?",
  "kv8.4:1.2_0": "Āmantā.",
  "kv8.4:1.3_0": "Nanu atthi tappaṭisaṁyutto chandoti?",
  "kv8.4:1.4_0": "Āmantā.",
  "kv8.4:1.5_0": "Hañci atthi tappaṭisaṁyutto chando, no ca vata re vattabbe—",
  "kv8.4:1.6_0": "“pañcevāyatanā kāmā”ti.",
  "kv8.4:1.7_0": "Nanu atthi tappaṭisaṁyutto rāgo tappaṭisaṁyutto chando tappaṭisaṁyutto chandarāgo tappaṭisaṁyutto saṅkappo tappaṭisaṁyutto rāgo tappaṭisaṁyutto saṅkapparāgo tappaṭisaṁyuttā pīti tappaṭisaṁyuttaṁ somanassaṁ tappaṭisaṁyuttaṁ pītisomanassanti?",
  "kv8.4:1.8_0": "Āmantā.",
  "kv8.4:1.9_0": "Hañci atthi tappaṭisaṁyuttaṁ pītisomanassaṁ, no ca vata re vattabbe—",
  "kv8.4:1.10_0": "“pañcevāyatanā kāmā”ti.",
  "kv8.4:2.1_0": "Na vattabbaṁ—",
  "kv8.4:2.2_0": "“pañcevāyatanā kāmā”ti?",
  "kv8.4:2.3_0": "Āmantā.",
  "kv8.4:2.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv8.4:2.5_0": "“pañcime, bhikkhave, kāmaguṇā.",
  "kv8.4:2.6_0": "Katame pañca?",
  "kv8.4:2.7_0": "Cakkhuviññeyyā rūpā …pe…",
  "kv8.4:2.8_0": "kāyaviññeyyā phoṭṭhabbā iṭṭhā kantā manāpā piyarūpā kāmūpasaṁhitā rajanīyā—",
  "kv8.4:2.9_0": "ime kho, bhikkhave, pañca kāmaguṇā”ti.",
  "kv8.4:2.10_0": "Attheva suttantoti?",
  "kv8.4:2.11_0": "Āmantā.",
  "kv8.4:2.12_0": "Tena hi pañcevāyatanā kāmāti.",
  "kv8.4:3.1_0": "Pañcevāyatanā kāmāti?",
  "kv8.4:3.2_0": "Āmantā.",
  "kv8.4:3.3_0": "Nanu vuttaṁ bhagavatā—",
  "kv8.4:3.4_0": "“pañcime, bhikkhave, kāmaguṇā.",
  "kv8.4:3.5_0": "Katame pañca?",
  "kv8.4:3.6_0": "Cakkhuviññeyyā rūpā …pe…",
  "kv8.4:3.7_0": "kāyaviññeyyā phoṭṭhabbā iṭṭhā kantā manāpā piyarūpā kāmūpasaṁhitā rajanīyā—",
  "kv8.4:3.8_0": "ime kho, bhikkhave, pañca kāmaguṇā.",
  "kv8.4:3.9_0": "Api ca, bhikkhave,",
  "kv8.4:3.9_1": "nete kāmā kāmaguṇā nāmete ariyassa vinaye vuccan”ti—",
  "kv8.4:4.1_0": "“Saṅkapparāgo purisassa kāmo,",
  "kv8.4:4.2_0": "Na te kāmā yāni citrāni loke;",
  "kv8.4:4.3_0": "Saṅkapparāgo purisassa kāmo,",
  "kv8.4:4.4_0": "Tiṭṭhanti citrāni tatheva loke;",
  "kv8.4:4.5_0": "Athettha dhīrā vinayanti chandan”ti.",
  "kv8.4:5.1_0": "Attheva suttantoti?",
  "kv8.4:5.2_0": "Āmantā.",
  "kv8.4:5.3_0": "Tena hi na vattabbaṁ—",
  "kv8.4:5.4_0": "“pañcevāyatanā kāmā”ti.",
  "kv8.4:5.5_0": "Kāmakathā niṭṭhitā."
}