{
  "atk-s0402a58:0_0": "(23) 3. Duccaritavaggavaṇṇanā",
  "atk-s0402a58:1_0": "221-231.",
  "atk-s0402a58:1_1": "Tatiyassa paṭhamādīni uttānatthāneva.",
  "atk-s0402a58:1_2": "Dasame yo cintetvā kabyaṁ karoti,",
  "atk-s0402a58:1_3": "ayaṁ cintākavi nāma.",
  "atk-s0402a58:1_4": "Yo sutvā karoti,",
  "atk-s0402a58:1_5": "ayaṁ sutakavi nāma.",
  "atk-s0402a58:1_6": "Yo ekaṁ atthaṁ nissāya karoti,",
  "atk-s0402a58:1_7": "ayaṁ atthakavi nāma.",
  "atk-s0402a58:1_8": "Yo taṅkhaṇaññeva vaṅgīsatthero viya attano paṭibhānena karoti,",
  "atk-s0402a58:1_9": "ayaṁ paṭibhānakavi nāmāti.",
  "atk-s0402a58:2_0": "Duccaritavaggo tatiyo."
}