{
  "ja455:0.1_0": "Jātaka",
  "ja455:0.2_0": "Ekādasakanipāta",
  "ja455:0.3_0": "Mātuposakavagga",
  "ja455:0.4_0": "1. Mātuposakajātaka",
  "ja455:1.1_0": "“Tassa nāgassa vippavāsena,",
  "ja455:1.2_0": "Virūḷhā sallakī ca kuṭajā ca;",
  "ja455:1.3_0": "Kuruvindakaravīrā bhisasāmā ca,",
  "ja455:1.4_0": "Nivāte pupphitā ca kaṇikārā.",
  "ja455:2.1_0": "Kocideva suvaṇṇakāyurā,",
  "ja455:2.2_0": "Nāgarājaṁ bharanti piṇḍena;",
  "ja455:2.3_0": "Yattha rājā rājakumāro vā,",
  "ja455:2.4_0": "Kavacamabhihessati achambhito”.",
  "ja455:3.1_0": "“Gaṇhāhi nāga kabaḷaṁ,",
  "ja455:3.2_0": "mā nāga kisako bhava;",
  "ja455:3.3_0": "Bahūni rājakiccāni,",
  "ja455:3.4_0": "tāni nāga karissasi”.",
  "ja455:4.1_0": "“Sā nūnasā kapaṇikā,",
  "ja455:4.2_0": "andhā apariṇāyikā;",
  "ja455:4.3_0": "Khāṇuṁ pādena ghaṭṭeti,",
  "ja455:4.4_0": "giriṁ caṇḍoraṇaṁ pati”.",
  "ja455:5.1_0": "“Kā nu te sā mahānāga,",
  "ja455:5.2_0": "andhā apariṇāyikā;",
  "ja455:5.3_0": "Khāṇuṁ pādena ghaṭṭeti,",
  "ja455:5.4_0": "giriṁ caṇḍoraṇaṁ pati”.",
  "ja455:6.1_0": "“Mātā me sā mahārāja,",
  "ja455:6.2_0": "andhā apariṇāyikā;",
  "ja455:6.3_0": "Khāṇuṁ pādena ghaṭṭeti,",
  "ja455:6.4_0": "giriṁ caṇḍoraṇaṁ pati”.",
  "ja455:7.1_0": "“Muñcathetaṁ mahānāgaṁ,",
  "ja455:7.2_0": "yoyaṁ bharati mātaraṁ;",
  "ja455:7.3_0": "Sametu mātarā nāgo,",
  "ja455:7.4_0": "saha sabbehi ñātibhi”.",
  "ja455:8.1_0": "“Mutto ca bandhanā nāgo,",
  "ja455:8.2_0": "muttamādāya kuñjaro;",
  "ja455:8.3_0": "Muhuttaṁ assāsayitvā,",
  "ja455:8.4_0": "agamā yena pabbato.",
  "ja455:9.1_0": "Tato so naḷiniṁ gantvā,",
  "ja455:9.2_0": "sītaṁ kuñjarasevitaṁ;",
  "ja455:9.3_0": "Soṇḍāyūdakamāhatvā,",
  "ja455:9.4_0": "mātaraṁ abhisiñcatha”.",
  "ja455:10.1_0": "“Koyaṁ anariyo devo,",
  "ja455:10.2_0": "akālenapi vassati;",
  "ja455:10.3_0": "Gato me atrajo putto,",
  "ja455:10.4_0": "yo mayhaṁ paricārako”.",
  "ja455:11.1_0": "“Uṭṭhehi amma kiṁ sesi,",
  "ja455:11.2_0": "āgato tyāhamatrajo;",
  "ja455:11.3_0": "Muttomhi kāsirājena,",
  "ja455:11.4_0": "vedehena yasassinā”.",
  "ja455:12.1_0": "“Ciraṁ jīvatu so rājā,",
  "ja455:12.2_0": "kāsīnaṁ raṭṭhavaḍḍhano;",
  "ja455:12.3_0": "Yo me puttaṁ pamocesi,",
  "ja455:12.4_0": "sadā vuddhāpacāyikan”ti.",
  "ja455:13.1_0": "Mātuposakajātakaṁ paṭhamaṁ."
}