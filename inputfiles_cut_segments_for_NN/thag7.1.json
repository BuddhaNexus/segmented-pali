{
  "thag7.1:0.1_0": "Theragāthā",
  "thag7.1:0.2_0": "Sattakanipāta",
  "thag7.1:0.3_0": "Paṭhamavagga",
  "thag7.1:0.4_0": "7.1. Sundarasamuddattheragāthā",
  "thag7.1:1.1_0": "“Alaṅkatā suvasanā,",
  "thag7.1:1.2_0": "māladhārī vibhūsitā;",
  "thag7.1:1.3_0": "Alattakakatāpādā,",
  "thag7.1:1.4_0": "pādukāruyha vesikā.",
  "thag7.1:2.1_0": "Pādukā oruhitvāna,",
  "thag7.1:2.2_0": "purato pañjalīkatā;",
  "thag7.1:2.3_0": "Sā maṁ saṇhena mudunā,",
  "thag7.1:2.4_0": "mhitapubbaṁ abhāsatha.",
  "thag7.1:3.1_0": "Yuvāsi tvaṁ pabbajito,",
  "thag7.1:3.2_0": "tiṭṭhāhi mama sāsane;",
  "thag7.1:3.3_0": "Bhuñja mānusake kāme,",
  "thag7.1:3.4_0": "ahaṁ vittaṁ dadāmi te;",
  "thag7.1:3.5_0": "Saccaṁ te paṭijānāmi,",
  "thag7.1:3.6_0": "aggiṁ vā te harāmahaṁ.",
  "thag7.1:4.1_0": "Yadā jiṇṇā bhavissāma,",
  "thag7.1:4.2_0": "ubho daṇḍaparāyanā;",
  "thag7.1:4.3_0": "Ubhopi pabbajissāma,",
  "thag7.1:4.4_0": "ubhayattha kaṭaggaho.",
  "thag7.1:5.1_0": "Tañca disvāna yācantiṁ,",
  "thag7.1:5.2_0": "vesikaṁ pañjalīkataṁ;",
  "thag7.1:5.3_0": "Alaṅkataṁ suvasanaṁ,",
  "thag7.1:5.4_0": "maccupāsaṁva oḍḍitaṁ.",
  "thag7.1:6.1_0": "Tato me manasīkāro,",
  "thag7.1:6.2_0": "…",
  "thag7.1:6.3_0": "pe…",
  "thag7.1:6.4_0": "nibbidā samatiṭṭhatha.",
  "thag7.1:7.1_0": "Tato cittaṁ vimucci me,",
  "thag7.1:7.2_0": "…",
  "thag7.1:7.3_0": "pe…",
  "thag7.1:7.4_0": "kataṁ buddhassa sāsanan”ti.",
  "thag7.1:7.5_0": "… Sundarasamuddo thero …."
}