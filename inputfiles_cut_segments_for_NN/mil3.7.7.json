{
  "mil3.7.7:0.1_0": "Milindapañha",
  "mil3.7.7:0.2_0": "Arūpadhammavavatthānavagga",
  "mil3.7.7:0.3_0": "7. Pāpapuññānaṁ appānappabhāvapañha",
  "mil3.7.7:1.1_0": "Rājā āha—",
  "mil3.7.7:1.2_0": "“bhante nāgasena, kataraṁ nu kho bahutaraṁ puññaṁ vā apuññaṁ vā”ti?",
  "mil3.7.7:1.3_0": "“Puññaṁ kho, mahārāja, bahutaraṁ, apuññaṁ thokan”ti.",
  "mil3.7.7:1.4_0": "“Kena kāraṇenā”ti?",
  "mil3.7.7:1.5_0": "“Apuññaṁ kho, mahārāja, karonto vippaṭisārī hoti ‘pāpakammaṁ mayā katan’ti, tena pāpaṁ na vaḍḍhati.",
  "mil3.7.7:1.6_0": "Puññaṁ kho, mahārāja,",
  "mil3.7.7:1.6_1": "karonto avippaṭisārī hoti,",
  "mil3.7.7:1.6_2": "avippaṭisārino pāmojjaṁ jāyati,",
  "mil3.7.7:1.6_3": "pamuditassa pīti jāyati,",
  "mil3.7.7:1.6_4": "pītimanassa kāyo passambhati,",
  "mil3.7.7:1.6_5": "passaddhakāyo sukhaṁ vedeti,",
  "mil3.7.7:1.6_6": "sukhino cittaṁ samādhiyati,",
  "mil3.7.7:1.6_7": "samāhito yathābhūtaṁ pajānāti,",
  "mil3.7.7:1.6_8": "tena kāraṇena puññaṁ vaḍḍhati.",
  "mil3.7.7:1.7_0": "Puriso kho, mahārāja,",
  "mil3.7.7:1.7_1": "chinnahatthapādo bhagavato ekaṁ uppalahatthaṁ datvā ekanavutikappāni vinipātaṁ na gacchissati.",
  "mil3.7.7:1.8_0": "Imināpi, mahārāja, kāraṇena bhaṇāmi ‘puññaṁ bahutaraṁ, apuññaṁ thokan’”ti.",
  "mil3.7.7:2.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.7.7:3.1_0": "Pāpapuññānaṁ appānappabhāvapañho sattamo."
}