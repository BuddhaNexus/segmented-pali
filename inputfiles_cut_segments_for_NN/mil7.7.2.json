{
  "mil7.7.2:0.1_0": "Milindapañha",
  "mil7.7.2:0.2_0": "Opammakathāpañha",
  "mil7.7.2:0.3_0": "Makkaṭakavagga",
  "mil7.7.2:0.4_0": "2. Thanassitadārakaṅgapañha",
  "mil7.7.2:1.1_0": "“Bhante nāgasena,",
  "mil7.7.2:1.1_1": "‘thanassitadārakassa ekaṁ aṅgaṁ gahetabban’ti yaṁ vadesi,",
  "mil7.7.2:1.1_2": "katamaṁ taṁ ekaṁ aṅgaṁ gahetabban”ti?",
  "mil7.7.2:1.2_0": "“Yathā, mahārāja,",
  "mil7.7.2:1.2_1": "thanassitadārako sadatthe laggati, khīratthiko rodati;",
  "mil7.7.2:1.3_0": "evameva kho, mahārāja,",
  "mil7.7.2:1.3_1": "yoginā yogāvacarena sadatthe laggitabbaṁ,",
  "mil7.7.2:1.3_2": "sabbattha dhammañāṇena bhavitabbaṁ,",
  "mil7.7.2:1.3_3": "uddese paripucchāya sammappayoge paviveke garusaṁvāse kalyāṇamittasevane.",
  "mil7.7.2:1.4_0": "Idaṁ, mahārāja,",
  "mil7.7.2:1.4_1": "thanassitadārakassa ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.7.2:1.5_0": "Bhāsitampetaṁ, mahārāja,",
  "mil7.7.2:1.5_1": "bhagavatā devātidevena dīghanikāyavare parinibbānasuttante—",
  "mil7.7.2:2.1_0": "‘Iṅgha tumhe, ānanda, sāratthe ghaṭatha, sāratthe anuyuñjatha,",
  "mil7.7.2:2.1_1": "sāratthe appamattā ātāpino pahitattā viharathā’”ti.",
  "mil7.7.2:3.1_0": "Thanassitadārakaṅgapañho dutiyo."
}