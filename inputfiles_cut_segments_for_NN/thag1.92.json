{
  "thag1.92:0.1_0": "Theragāthā",
  "thag1.92:0.2_0": "Ekakanipāta",
  "thag1.92:0.3_0": "Dasamavagga",
  "thag1.92:0.4_0": "1.92. Vijayattheragāthā",
  "thag1.92:1.1_0": "“Yassāsavā parikkhīṇā,",
  "thag1.92:1.2_0": "āhāre ca anissito;",
  "thag1.92:1.3_0": "Suññato animitto ca,",
  "thag1.92:1.4_0": "vimokkho yassa gocaro;",
  "thag1.92:1.5_0": "Ākāseva sakuntānaṁ,",
  "thag1.92:1.6_0": "padaṁ tassa durannayan”ti.",
  "thag1.92:1.7_0": "… Vijayo thero …."
}