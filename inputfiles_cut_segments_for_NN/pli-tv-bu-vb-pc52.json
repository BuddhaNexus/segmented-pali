{
  "pli-tv-bu-vb-pc52:0.1_0": "Theravāda Vinaya",
  "pli-tv-bu-vb-pc52:0.2_0": "Mahāvibhaṅga",
  "pli-tv-bu-vb-pc52:0.3_0": "Pācittiyakaṇḍa",
  "pli-tv-bu-vb-pc52:0.4_0": "Surāpānavagga",
  "pli-tv-bu-vb-pc52:0.5_0": "52. Aṅgulipatodakasikkhāpada",
  "pli-tv-bu-vb-pc52:1.1_0": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-pc52:1.2_0": "Tena kho pana samayena chabbaggiyā bhikkhū sattarasavaggiyaṁ bhikkhuṁ aṅgulipatodakena hāsesuṁ.",
  "pli-tv-bu-vb-pc52:1.3_0": "So bhikkhu uttanto anassāsako kālamakāsi.",
  "pli-tv-bu-vb-pc52:1.4_0": "Ye te bhikkhū appicchā …pe… te ujjhāyanti khiyyanti vipācenti—",
  "pli-tv-bu-vb-pc52:1.5_0": "“kathañhi nāma chabbaggiyā bhikkhū bhikkhuṁ aṅgulipatodakena hāsessantī”ti …pe…",
  "pli-tv-bu-vb-pc52:1.6_0": "“saccaṁ kira tumhe, bhikkhave,",
  "pli-tv-bu-vb-pc52:1.6_1": "bhikkhuṁ aṅgulipatodakena hāsethā”ti?",
  "pli-tv-bu-vb-pc52:1.7_0": "“Saccaṁ, bhagavā”ti.",
  "pli-tv-bu-vb-pc52:1.8_0": "Vigarahi buddho bhagavā …pe…",
  "pli-tv-bu-vb-pc52:1.9_0": "kathañhi nāma tumhe, moghapurisā,",
  "pli-tv-bu-vb-pc52:1.9_1": "bhikkhuṁ aṅgulipatodakena hāsessatha.",
  "pli-tv-bu-vb-pc52:1.10_0": "Netaṁ, moghapurisā, appasannānaṁ vā pasādāya …pe…",
  "pli-tv-bu-vb-pc52:1.11_0": "evañca pana, bhikkhave,",
  "pli-tv-bu-vb-pc52:1.11_1": "imaṁ sikkhāpadaṁ uddiseyyātha—",
  "pli-tv-bu-vb-pc52:1.12.1_0": "“Aṅgulipatodake pācittiyan”ti.",
  "pli-tv-bu-vb-pc52:2.1.1_0": "Aṅgulipatodako nāma",
  "pli-tv-bu-vb-pc52:2.1.2_0": "upasampanno upasampannaṁ hasādhippāyo kāyena kāyaṁ āmasati, āpatti pācittiyassa.",
  "pli-tv-bu-vb-pc52:2.2.1_0": "Upasampanne upasampannasaññī aṅgulipatodakena hāseti, āpatti pācittiyassa.",
  "pli-tv-bu-vb-pc52:2.2.2_0": "Upasampanne vematiko aṅgulipatodakena hāseti, āpatti pācittiyassa.",
  "pli-tv-bu-vb-pc52:2.2.3_0": "Upasampanne anupasampannasaññī aṅgulipatodakena hāseti, āpatti pācittiyassa.",
  "pli-tv-bu-vb-pc52:2.2.4_0": "Kāyena kāyapaṭibaddhaṁ āmasati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-pc52:2.2.5_0": "Kāyapaṭibaddhena kāyaṁ āmasati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-pc52:2.2.6_0": "Kāyapaṭibaddhena kāyapaṭibaddhaṁ āmasati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-pc52:2.2.7_0": "Nissaggiyena kāyaṁ āmasati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-pc52:2.2.8_0": "Nissaggiyena kāyapaṭibaddhaṁ āmasati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-pc52:2.2.9_0": "Nissaggiyena nissaggiyaṁ āmasati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-pc52:2.2.10_0": "Anupasampannaṁ kāyena kāyaṁ āmasati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-pc52:2.2.11_0": "Kāyena kāyapaṭibaddhaṁ āmasati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-pc52:2.2.12_0": "Kāyapaṭibaddhena kāyaṁ āmasati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-pc52:2.2.13_0": "Kāyapaṭibaddhena kāyapaṭibaddhaṁ āmasati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-pc52:2.2.14_0": "Nissaggiyena kāyaṁ āmasati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-pc52:2.2.15_0": "Nissaggiyena kāyapaṭibaddhaṁ āmasati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-pc52:2.2.16_0": "Nissaggiyena nissaggiyaṁ āmasati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-pc52:2.2.17_0": "Anupasampanne upasampannasaññī, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-pc52:2.2.18_0": "Anupasampanne vematiko, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-pc52:2.2.19_0": "Anupasampanne anupasampannasaññī, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-pc52:2.3.1_0": "Anāpatti—",
  "pli-tv-bu-vb-pc52:2.3.2_0": "na hasādhippāyo,",
  "pli-tv-bu-vb-pc52:2.3.3_0": "sati karaṇīye āmasati,",
  "pli-tv-bu-vb-pc52:2.3.4_0": "ummattakassa,",
  "pli-tv-bu-vb-pc52:2.3.5_0": "ādikammikassāti.",
  "pli-tv-bu-vb-pc52:2.3.6_0": "Aṅgulipatodakasikkhāpadaṁ niṭṭhitaṁ dutiyaṁ."
}