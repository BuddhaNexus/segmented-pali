{
  "tha-ap518:0.1_0": "Therāpadāna",
  "tha-ap518:0.2_0": "Phaladāyakavagga",
  "tha-ap518:0.3_0": "6. Ajjunapupphiyattheraapadāna",
  "tha-ap518:1.1_0": "“Candabhāgānadītīre,",
  "tha-ap518:1.2_0": "ahosiṁ kinnaro tadā;",
  "tha-ap518:1.3_0": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap518:1.4_0": "sayambhuṁ aparājitaṁ.",
  "tha-ap518:2.1_0": "Pasannacitto sumano,",
  "tha-ap518:2.2_0": "vedajāto katañjalī;",
  "tha-ap518:2.3_0": "Gahetvā ajjunaṁ pupphaṁ,",
  "tha-ap518:2.4_0": "sayambhuṁ abhipūjayiṁ.",
  "tha-ap518:3.1_0": "Tena kammena sukatena,",
  "tha-ap518:3.2_0": "cetanāpaṇidhīhi ca;",
  "tha-ap518:3.3_0": "Jahitvā kinnaraṁ dehaṁ,",
  "tha-ap518:3.4_0": "tāvatiṁsamagacchahaṁ.",
  "tha-ap518:4.1_0": "Chattiṁsakkhattuṁ devindo,",
  "tha-ap518:4.2_0": "devarajjamakārayiṁ;",
  "tha-ap518:4.3_0": "Dasakkhattuṁ cakkavattī,",
  "tha-ap518:4.4_0": "mahārajjamakārayiṁ.",
  "tha-ap518:5.1_0": "Padesarajjaṁ vipulaṁ,",
  "tha-ap518:5.2_0": "gaṇanāto asaṅkhiyaṁ;",
  "tha-ap518:5.3_0": "Sukhette vappitaṁ bījaṁ,",
  "tha-ap518:5.4_0": "sayambhumhi aho mama.",
  "tha-ap518:6.1_0": "Kusalaṁ vijjate mayhaṁ,",
  "tha-ap518:6.2_0": "pabbajiṁ anagāriyaṁ;",
  "tha-ap518:6.3_0": "Pūjāraho ahaṁ ajja,",
  "tha-ap518:6.4_0": "sakyaputtassa sāsane.",
  "tha-ap518:7.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap518:7.2_0": "…pe…",
  "tha-ap518:7.3_0": "viharāmi anāsavo.",
  "tha-ap518:8.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap518:8.2_0": "…pe…",
  "tha-ap518:8.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap518:9.1_0": "Paṭisambhidā catasso,",
  "tha-ap518:9.2_0": "…pe…",
  "tha-ap518:9.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap518:10.1_0": "Itthaṁ sudaṁ āyasmā ajjunapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap518:11.1_0": "Ajjunapupphiyattherassāpadānaṁ chaṭṭhaṁ."
}