{
  "kv5.9:0.1_0": "Kathāvatthu",
  "kv5.9:0.2_0": "Mahāpaṇṇāsaka",
  "kv5.9:0.3_0": "Pañcamavagga",
  "kv5.9:0.4_0": "Paṭuppannañāṇakathā",
  "kv5.9:1.1_0": "Paṭuppanne ñāṇaṁ atthīti?",
  "kv5.9:1.2_0": "Āmantā.",
  "kv5.9:1.3_0": "Tena ñāṇena taṁ ñāṇaṁ jānātīti?",
  "kv5.9:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv5.9:1.5_0": "tena ñāṇena taṁ ñāṇaṁ jānātīti?",
  "kv5.9:1.6_0": "Āmantā.",
  "kv5.9:1.7_0": "Tena ñāṇena taṁ ñāṇaṁ “ñāṇan”ti jānātīti?",
  "kv5.9:1.8_0": "Na hevaṁ vattabbe …pe…",
  "kv5.9:1.9_0": "tena ñāṇena taṁ ñāṇaṁ “ñāṇan”ti jānātīti?",
  "kv5.9:1.10_0": "Āmantā.",
  "kv5.9:1.11_0": "Taṁ ñāṇaṁ tassa ñāṇassa ārammaṇanti?",
  "kv5.9:1.12_0": "Na hevaṁ vattabbe …pe….",
  "kv5.9:2.1_0": "Taṁ ñāṇaṁ tassa ñāṇassa ārammaṇanti?",
  "kv5.9:2.2_0": "Āmantā.",
  "kv5.9:2.3_0": "Tena phassena taṁ phassaṁ phusati,",
  "kv5.9:2.3_1": "tāya vedanāya taṁ vedanaṁ vedeti,",
  "kv5.9:2.3_2": "tāya saññāya taṁ saññaṁ sañjānāti,",
  "kv5.9:2.3_3": "tāya cetanāya taṁ cetanaṁ ceteti,",
  "kv5.9:2.3_4": "tena cittena taṁ cittaṁ cinteti,",
  "kv5.9:2.3_5": "tena vitakkena taṁ vitakkaṁ vitakketi,",
  "kv5.9:2.3_6": "tena vicārena taṁ vicāraṁ vicāreti,",
  "kv5.9:2.3_7": "tāya pītiyā taṁ pītiṁ piyāyati,",
  "kv5.9:2.3_8": "tāya satiyā taṁ satiṁ sarati,",
  "kv5.9:2.3_9": "tāya paññāya taṁ paññaṁ pajānāti,",
  "kv5.9:2.3_10": "tena khaggena taṁ khaggaṁ chindati,",
  "kv5.9:2.3_11": "tena pharasunā taṁ pharasuṁ tacchati,",
  "kv5.9:2.3_12": "tāya kudhāriyā taṁ kudhāriṁ tacchati,",
  "kv5.9:2.3_13": "tāya vāsiyā taṁ vāsiṁ tacchati,",
  "kv5.9:2.3_14": "tāya sūciyā taṁ sūciṁ sibbeti,",
  "kv5.9:2.3_15": "tena aṅgulaggena taṁ aṅgulaggaṁ parāmasati,",
  "kv5.9:2.3_16": "tena nāsikaggena taṁ nāsikaggaṁ parāmasati,",
  "kv5.9:2.3_17": "tena matthakena taṁ matthakaṁ parāmasati,",
  "kv5.9:2.3_18": "tena gūthena taṁ gūthaṁ dhovati,",
  "kv5.9:2.3_19": "tena muttena taṁ muttaṁ dhovati,",
  "kv5.9:2.3_20": "tena kheḷena taṁ kheḷaṁ dhovati,",
  "kv5.9:2.3_21": "tena pubbena taṁ pubbaṁ dhovati,",
  "kv5.9:2.3_22": "tena lohitena taṁ lohitaṁ dhovatīti?",
  "kv5.9:2.4_0": "Na hevaṁ vattabbe …pe….",
  "kv5.9:3.1_0": "Na vattabbaṁ—",
  "kv5.9:3.2_0": "“paṭuppanne ñāṇaṁ atthī”ti?",
  "kv5.9:3.3_0": "Āmantā.",
  "kv5.9:3.4_0": "Nanu sabbasaṅkhāre aniccato diṭṭhe tampi ñāṇaṁ aniccato diṭṭhaṁ hotīti?",
  "kv5.9:3.5_0": "Āmantā.",
  "kv5.9:3.6_0": "Hañci sabbasaṅkhāre aniccato diṭṭhe tampi ñāṇaṁ aniccato diṭṭhaṁ hoti, tena vata re vattabbe—",
  "kv5.9:3.7_0": "“paṭuppanne ñāṇaṁ atthī”ti.",
  "kv5.9:3.8_0": "Paṭuppannañāṇakathā niṭṭhitā."
}