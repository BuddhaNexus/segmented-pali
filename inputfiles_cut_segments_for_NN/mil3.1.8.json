{
  "mil3.1.8:0.1_0": "Milindapañha",
  "mil3.1.8:0.2_0": "Mahāvagga",
  "mil3.1.8:0.3_0": "8. Manasikāralakkhaṇapañha",
  "mil3.1.8:1.1_0": "Rājā āha—",
  "mil3.1.8:1.2_0": "“kiṁlakkhaṇo, bhante nāgasena, manasikāro, kiṁlakkhaṇā paññā”ti?",
  "mil3.1.8:1.3_0": "“Ūhanalakkhaṇo kho, mahārāja, manasikāro, chedanalakkhaṇā paññā”ti.",
  "mil3.1.8:2.1_0": "“Kathaṁ ūhanalakkhaṇo manasikāro, kathaṁ chedanalakkhaṇā paññā?",
  "mil3.1.8:2.2_0": "Opammaṁ karohī”ti.",
  "mil3.1.8:2.3_0": "“Jānāsi tvaṁ, mahārāja, yavalāvake”ti.",
  "mil3.1.8:2.4_0": "“Āma, bhante, jānāmī”ti.",
  "mil3.1.8:2.5_0": "“Kathaṁ, mahārāja, yavalāvakā yavaṁ lunantī”ti?",
  "mil3.1.8:2.6_0": "“Vāmena, bhante,",
  "mil3.1.8:2.6_1": "hatthena yavakalāpaṁ gahetvā dakkhiṇena hatthena dāttaṁ gahetvā dāttena chindantī”ti.",
  "mil3.1.8:3.1_0": "“Yathā, mahārāja,",
  "mil3.1.8:3.1_1": "yavalāvako vāmena hatthena yavakalāpaṁ gahetvā dakkhiṇena hatthena dāttaṁ gahetvā yavaṁ chindati;",
  "mil3.1.8:3.2_0": "evameva kho, mahārāja,",
  "mil3.1.8:3.2_1": "yogāvacaro manasikārena mānasaṁ gahetvā paññāya kilese chindati, evaṁ kho, mahārāja, ūhanalakkhaṇo manasikāro,",
  "mil3.1.8:3.2_2": "evaṁ chedanalakkhaṇā paññā”ti.",
  "mil3.1.8:4.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.1.8:5.1_0": "Manasikāralakkhaṇapañho aṭṭhamo."
}