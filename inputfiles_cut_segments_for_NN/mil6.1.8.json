{
  "mil6.1.8:0.1_0": "Milindapañha",
  "mil6.1.8:0.2_0": "Anumānapañha",
  "mil6.1.8:0.3_0": "Buddhavagga",
  "mil6.1.8:0.4_0": "8. Dussīlapañha",
  "mil6.1.8:1.1_0": "“Bhante nāgasena,",
  "mil6.1.8:1.1_1": "gihidussīlassa ca samaṇadussīlassa ca ko viseso, kiṁ nānākaraṇaṁ, ubhopete samasamagatikā,",
  "mil6.1.8:1.1_2": "ubhinnampi samasamo vipāko hoti,",
  "mil6.1.8:1.1_3": "udāhu kiñci nānākāraṇaṁ atthī”ti?",
  "mil6.1.8:2.1_0": "“Dasayime, mahārāja,",
  "mil6.1.8:2.1_1": "guṇā samaṇadussīlassa gihidussīlato visesena atirekā,",
  "mil6.1.8:2.1_2": "dasahi ca kāraṇehi uttariṁ dakkhiṇaṁ visodheti.",
  "mil6.1.8:3.1_0": "Katame dasa guṇā samaṇadussīlassa gihidussīlato visesena atirekā?",
  "mil6.1.8:3.2_0": "Idha, mahārāja,",
  "mil6.1.8:3.2_1": "samaṇadussīlo buddhe sagāravo hoti, dhamme sagāravo hoti, saṅghe sagāravo hoti,",
  "mil6.1.8:3.2_2": "sabrahmacārīsu sagāravo hoti, uddesaparipucchāya vāyamati, savanabahulo hoti, bhinnasīlopi, mahārāja,",
  "mil6.1.8:3.2_3": "dussīlo parisagato ākappaṁ upaṭṭhapeti,",
  "mil6.1.8:3.2_4": "garahabhayā kāyikaṁ vācasikaṁ rakkhati,",
  "mil6.1.8:3.2_5": "padhānābhimukhañcassa hoti cittaṁ, bhikkhusāmaññaṁ upagato hoti.",
  "mil6.1.8:3.3_0": "Karontopi, mahārāja,",
  "mil6.1.8:3.3_1": "samaṇadussīlo pāpaṁ paṭicchannaṁ ācarati.",
  "mil6.1.8:3.4_0": "Yathā, mahārāja,",
  "mil6.1.8:3.4_1": "itthī sapatikā nilīyitvā rahasseneva pāpamācarati;",
  "mil6.1.8:3.5_0": "evameva kho, mahārāja,",
  "mil6.1.8:3.5_1": "karontopi samaṇadussīlo pāpaṁ paṭicchannaṁ ācarati.",
  "mil6.1.8:3.6_0": "Ime kho, mahārāja,",
  "mil6.1.8:3.6_1": "dasa guṇā samaṇadussīlassa gihidussīlato visesena atirekā.",
  "mil6.1.8:4.1_0": "Katamehi dasahi kāraṇehi uttariṁ dakkhiṇaṁ visodheti?",
  "mil6.1.8:4.2_0": "Anavajjakavacadhāraṇatāyapi dakkhiṇaṁ visodheti,",
  "mil6.1.8:4.2_1": "isisāmaññabhaṇḍuliṅgadhāraṇatopi dakkhiṇaṁ visodheti,",
  "mil6.1.8:4.2_2": "saṅghasamayamanuppaviṭṭhatāyapi dakkhiṇaṁ visodheti,",
  "mil6.1.8:4.2_3": "buddhadhammasaṅghasaraṇagatatāyapi dakkhiṇaṁ visodheti,",
  "mil6.1.8:4.2_4": "padhānāsayaniketavāsitāyapi dakkhiṇaṁ visodheti,",
  "mil6.1.8:4.2_5": "jinasāsanadharapariyesanatopi dakkhiṇaṁ visodheti,",
  "mil6.1.8:4.2_6": "pavaradhammadesanatopi dakkhiṇaṁ visodheti,",
  "mil6.1.8:4.2_7": "dhammadīpagatiparāyaṇatāyapi dakkhiṇaṁ visodheti,",
  "mil6.1.8:4.2_8": "‘aggo buddho’ti ekantaujudiṭṭhitāyapi dakkhiṇaṁ visodheti,",
  "mil6.1.8:4.2_9": "uposathasamādānatopi dakkhiṇaṁ visodheti.",
  "mil6.1.8:4.3_0": "Imehi kho, mahārāja,",
  "mil6.1.8:4.3_1": "dasahi kāraṇehi uttariṁ dakkhiṇaṁ visodheti.",
  "mil6.1.8:5.1_0": "Suvipannopi hi, mahārāja,",
  "mil6.1.8:5.1_1": "samaṇadussīlo dāyakānaṁ dakkhiṇaṁ visodheti.",
  "mil6.1.8:5.2_0": "Yathā, mahārāja,",
  "mil6.1.8:5.2_1": "udakaṁ subahalampi kalalakaddamarajojallaṁ apaneti;",
  "mil6.1.8:5.3_0": "evameva kho, mahārāja,",
  "mil6.1.8:5.3_1": "suvipannopi samaṇadussīlo dāyakānaṁ dakkhiṇaṁ visodheti.",
  "mil6.1.8:6.1_0": "Yathā vā pana, mahārāja,",
  "mil6.1.8:6.1_1": "uṇhodakaṁ sukudhitampi pajjalantaṁ mahantaṁ aggikkhandhaṁ nibbāpeti;",
  "mil6.1.8:6.2_0": "evameva kho, mahārāja,",
  "mil6.1.8:6.2_1": "suvipannopi samaṇadussīlo dāyakānaṁ dakkhiṇaṁ visodheti.",
  "mil6.1.8:7.1_0": "Yathā vā pana, mahārāja,",
  "mil6.1.8:7.1_1": "bhojanaṁ virasampi khudādubbalyaṁ apaneti;",
  "mil6.1.8:7.2_0": "evameva kho, mahārāja,",
  "mil6.1.8:7.2_1": "suvipannopi samaṇadussīlo dāyakānaṁ dakkhiṇaṁ visodheti.",
  "mil6.1.8:8.1_0": "Bhāsitampetaṁ, mahārāja,",
  "mil6.1.8:8.1_1": "tathāgatena devātidevena majjhimanikāyavaralañchake dakkhiṇavibhaṅge veyyākaraṇe—",
  "mil6.1.8:9.1_0": "‘Yo sīlavā dussīlesu dadāti dānaṁ,",
  "mil6.1.8:9.2_0": "Dhammena laddhaṁ supasannacitto;",
  "mil6.1.8:9.3_0": "Abhisaddahaṁ kammaphalaṁ uḷāraṁ,",
  "mil6.1.8:9.4_0": "Sā dakkhiṇā dāyakato visujjhatī’”ti.",
  "mil6.1.8:10.1_0": "“Acchariyaṁ, bhante nāgasena, abbhutaṁ, bhante nāgasena,",
  "mil6.1.8:10.1_1": "tāvatakaṁ mayaṁ pañhaṁ apucchimha,",
  "mil6.1.8:10.1_2": "taṁ tvaṁ opammehi kāraṇehi vibhāvento amatamadhuraṁ savanūpagaṁ akāsi.",
  "mil6.1.8:10.2_0": "Yathā nāma, bhante,",
  "mil6.1.8:10.2_1": "sūdo vā sūdantevāsī vā tāvatakaṁ maṁsaṁ labhitvā nānāvidhehi sambhārehi sampādetvā rājūpabhogaṁ karoti;",
  "mil6.1.8:10.3_0": "evameva kho, bhante nāgasena,",
  "mil6.1.8:10.3_1": "tāvatakaṁ mayaṁ pañhaṁ apucchimha,",
  "mil6.1.8:10.3_2": "taṁ tvaṁ opammehi kāraṇehi vibhāvetvā amatamadhuraṁ savanūpagaṁ akāsī”ti.",
  "mil6.1.8:11.1_0": "Dussīlapañho aṭṭhamo."
}