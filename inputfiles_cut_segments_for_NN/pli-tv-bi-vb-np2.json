{
  "pli-tv-bi-vb-np2:0.1_0": "Theravāda Vinaya",
  "pli-tv-bi-vb-np2:0.2_0": "Bhikkhunivibhaṅga",
  "pli-tv-bi-vb-np2:0.3_0": "Nissaggiyakaṇḍa",
  "pli-tv-bi-vb-np2:0.4_0": "Pattavagga",
  "pli-tv-bi-vb-np2:0.5_0": "2. Akālacīvarabhājanasikkhāpada",
  "pli-tv-bi-vb-np2:1.1_0": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bi-vb-np2:1.2_0": "Tena kho pana samayena sambahulā bhikkhuniyo gāmakāvāse vassaṁvuṭṭhā sāvatthiṁ agamaṁsu vattasampannā iriyāpathasampannā duccoḷā lūkhacīvarā.",
  "pli-tv-bi-vb-np2:1.3_0": "Upāsakā tā bhikkhuniyo passitvā—",
  "pli-tv-bi-vb-np2:1.4_0": "“imā bhikkhuniyo vattasampannā iriyāpathasampannā duccoḷā lūkhacīvarā,",
  "pli-tv-bi-vb-np2:1.4_1": "imā bhikkhuniyo acchinnā bhavissantī”ti bhikkhunisaṅghassa akālacīvaraṁ adaṁsu.",
  "pli-tv-bi-vb-np2:1.5_0": "Thullanandā bhikkhunī—",
  "pli-tv-bi-vb-np2:1.6_0": "“amhākaṁ kathinaṁ atthataṁ kālacīvaran”ti adhiṭṭhahitvā bhājāpesi.",
  "pli-tv-bi-vb-np2:1.7_0": "Upāsakā tā bhikkhuniyo passitvā etadavocuṁ—",
  "pli-tv-bi-vb-np2:1.8_0": "“apayyāhi cīvaraṁ laddhan”ti?",
  "pli-tv-bi-vb-np2:1.9_0": "“Na mayaṁ, āvuso, cīvaraṁ labhāma.",
  "pli-tv-bi-vb-np2:1.10_0": "Ayyā thullanandā—",
  "pli-tv-bi-vb-np2:1.11_0": "‘amhākaṁ kathinaṁ atthataṁ kālacīvaran’ti adhiṭṭhahitvā bhājāpesī”ti.",
  "pli-tv-bi-vb-np2:1.12_0": "Upāsakā ujjhāyanti khiyyanti vipācenti—",
  "pli-tv-bi-vb-np2:1.13_0": "“kathañhi nāma ayyā thullanandā akālacīvaraṁ ‘kālacīvaran’ti adhiṭṭhahitvā bhājāpessatī”ti.",
  "pli-tv-bi-vb-np2:1.14_0": "Assosuṁ kho bhikkhuniyo tesaṁ upāsakānaṁ ujjhāyantānaṁ khiyyantānaṁ vipācentānaṁ.",
  "pli-tv-bi-vb-np2:1.15_0": "Yā tā bhikkhuniyo appicchā …pe… tā ujjhāyanti khiyyanti vipācenti—",
  "pli-tv-bi-vb-np2:1.16_0": "“kathañhi nāma ayyā thullanandā akālacīvaraṁ ‘kālacīvaran’ti adhiṭṭhahitvā bhājāpessatī”ti.",
  "pli-tv-bi-vb-np2:1.17_0": "Atha kho tā bhikkhuniyo bhikkhūnaṁ etamatthaṁ ārocesuṁ.",
  "pli-tv-bi-vb-np2:1.18_0": "Bhikkhū bhagavato etamatthaṁ ārocesuṁ …pe…",
  "pli-tv-bi-vb-np2:1.19_0": "“saccaṁ kira,",
  "pli-tv-bi-vb-np2:1.19_1": "bhikkhave,",
  "pli-tv-bi-vb-np2:1.19_2": "thullanandā bhikkhunī akālacīvaraṁ ‘kālacīvaran’ti adhiṭṭhahitvā bhājāpetī”ti?",
  "pli-tv-bi-vb-np2:1.20_0": "“Saccaṁ, bhagavā”ti.",
  "pli-tv-bi-vb-np2:1.21_0": "Vigarahi buddho bhagavā …pe…",
  "pli-tv-bi-vb-np2:1.22_0": "kathañhi nāma,",
  "pli-tv-bi-vb-np2:1.22_1": "bhikkhave,",
  "pli-tv-bi-vb-np2:1.22_2": "thullanandā bhikkhunī akālacīvaraṁ “kālacīvaran”ti adhiṭṭhahitvā bhājāpessati.",
  "pli-tv-bi-vb-np2:1.23_0": "Netaṁ, bhikkhave, appasannānaṁ vā pasādāya …pe…",
  "pli-tv-bi-vb-np2:1.24_0": "evañca pana, bhikkhave, bhikkhuniyo imaṁ sikkhāpadaṁ uddisantu—",
  "pli-tv-bi-vb-np2:1.25.1_0": "“Yā pana bhikkhunī akālacīvaraṁ ‘kālacīvaran’ti adhiṭṭhahitvā bhājāpeyya, nissaggiyaṁ pācittiyan”ti.",
  "pli-tv-bi-vb-np2:2.1.1_0": "Yā panāti",
  "pli-tv-bi-vb-np2:2.1.2_0": "yā yādisā …pe…",
  "pli-tv-bi-vb-np2:2.1.3_0": "bhikkhunīti",
  "pli-tv-bi-vb-np2:2.1.4_0": "…pe… ayaṁ imasmiṁ atthe adhippetā bhikkhunīti.",
  "pli-tv-bi-vb-np2:2.1.5_0": "Akālacīvaraṁ nāma",
  "pli-tv-bi-vb-np2:2.1.6_0": "anatthate kathine ekādasamāse uppannaṁ,",
  "pli-tv-bi-vb-np2:2.1.6_1": "atthate kathine sattamāse uppannaṁ,",
  "pli-tv-bi-vb-np2:2.1.6_2": "kālepi ādissa dinnaṁ,",
  "pli-tv-bi-vb-np2:2.1.6_3": "etaṁ akālacīvaraṁ nāma.",
  "pli-tv-bi-vb-np2:2.1.7_0": "Akālacīvaraṁ “kālacīvaran”ti adhiṭṭhahitvā bhājāpeti, payoge dukkaṭaṁ.",
  "pli-tv-bi-vb-np2:2.1.8_0": "Paṭilābhena nissaggiyaṁ hoti.",
  "pli-tv-bi-vb-np2:2.1.9_0": "Nissajjitabbaṁ saṅghassa vā gaṇassa vā ekabhikkhuniyā vā.",
  "pli-tv-bi-vb-np2:2.1.10_0": "Evañca pana, bhikkhave, nissajjitabbaṁ.",
  "pli-tv-bi-vb-np2:2.1.11_0": "…pe…",
  "pli-tv-bi-vb-np2:2.1.12_0": "“idaṁ me, ayye,",
  "pli-tv-bi-vb-np2:2.1.12_1": "akālacīvaraṁ ‘kālacīvaran’ti adhiṭṭhahitvā bhājāpitaṁ nissaggiyaṁ,",
  "pli-tv-bi-vb-np2:2.1.12_2": "imāhaṁ saṅghassa nissajjāmī”ti …pe…",
  "pli-tv-bi-vb-np2:2.1.13_0": "dadeyyā”ti …pe…",
  "pli-tv-bi-vb-np2:2.1.14_0": "dadeyyun”ti …pe…",
  "pli-tv-bi-vb-np2:2.1.15_0": "ayyāya dammī”ti.",
  "pli-tv-bi-vb-np2:2.2.1_0": "Akālacīvare akālacīvarasaññā “kālacīvaran”ti adhiṭṭhahitvā bhājāpeti, nissaggiyaṁ pācittiyaṁ.",
  "pli-tv-bi-vb-np2:2.2.2_0": "Akālacīvare vematikā “kālacīvaran”ti adhiṭṭhahitvā bhājāpeti, āpatti dukkaṭassa.",
  "pli-tv-bi-vb-np2:2.2.3_0": "Akālacīvare kālacīvarasaññā “kālacīvaran”ti adhiṭṭhahitvā bhājāpeti, anāpatti.",
  "pli-tv-bi-vb-np2:2.2.4_0": "Kālacīvare akālacīvarasaññā, āpatti dukkaṭassa.",
  "pli-tv-bi-vb-np2:2.2.5_0": "Kālacīvare vematikā, āpatti dukkaṭassa.",
  "pli-tv-bi-vb-np2:2.2.6_0": "Kālacīvare kālacīvarasaññā, anāpatti.",
  "pli-tv-bi-vb-np2:2.3.1_0": "Anāpatti—",
  "pli-tv-bi-vb-np2:2.3.2_0": "akālacīvaraṁ kālacīvarasaññā bhājāpeti,",
  "pli-tv-bi-vb-np2:2.3.3_0": "kālacīvaraṁ kālacīvarasaññā bhājāpeti,",
  "pli-tv-bi-vb-np2:2.3.4_0": "ummattikāya,",
  "pli-tv-bi-vb-np2:2.3.5_0": "ādikammikāyāti.",
  "pli-tv-bi-vb-np2:2.3.6_0": "Dutiyasikkhāpadaṁ niṭṭhitaṁ."
}