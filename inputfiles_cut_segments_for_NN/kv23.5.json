{
  "kv23.5:0.1_0": "Kathāvatthu",
  "kv23.5:0.2_0": "Mahāpaṇṇāsaka",
  "kv23.5:0.3_0": "Tevīsatimavagga",
  "kv23.5:0.4_0": "Aparinipphannakathā",
  "kv23.5:1.1_0": "Rūpaṁ aparinipphannanti?",
  "kv23.5:1.2_0": "Āmantā.",
  "kv23.5:1.3_0": "Rūpaṁ na aniccaṁ na saṅkhataṁ na paṭiccasamuppannaṁ na khayadhammaṁ na vayadhammaṁ na virāgadhammaṁ na nirodhadhammaṁ na vipariṇāmadhammanti?",
  "kv23.5:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv23.5:1.5_0": "nanu rūpaṁ aniccaṁ saṅkhataṁ paṭiccasamuppannaṁ khayadhammaṁ vayadhammaṁ virāgadhammaṁ nirodhadhammaṁ vipariṇāmadhammanti?",
  "kv23.5:1.6_0": "Āmantā.",
  "kv23.5:1.7_0": "Hañci rūpaṁ aniccaṁ saṅkhataṁ …pe…",
  "kv23.5:1.8_0": "vipariṇāmadhammaṁ, no ca vata re vattabbe—",
  "kv23.5:1.9_0": "“rūpaṁ aparinipphannan”ti.",
  "kv23.5:2.1_0": "Dukkhaññeva parinipphannanti?",
  "kv23.5:2.2_0": "Āmantā.",
  "kv23.5:2.3_0": "Nanu yadaniccaṁ taṁ dukkhaṁ vuttaṁ bhagavatā—",
  "kv23.5:2.4_0": "“rūpaṁ aniccan”ti?",
  "kv23.5:2.5_0": "Āmantā.",
  "kv23.5:2.6_0": "Hañci yadaniccaṁ taṁ dukkhaṁ vuttaṁ bhagavatā—",
  "kv23.5:2.7_0": "“rūpaṁ aniccaṁ”, no ca vata re vattabbe—",
  "kv23.5:2.8_0": "“dukkhaññeva parinipphannan”ti …pe….",
  "kv23.5:3.1_0": "Vedanā …pe…",
  "kv23.5:3.2_0": "saññā …",
  "kv23.5:3.3_0": "saṅkhārā …",
  "kv23.5:3.4_0": "viññāṇaṁ …",
  "kv23.5:3.5_0": "cakkhāyatanaṁ …pe…",
  "kv23.5:3.6_0": "dhammāyatanaṁ …",
  "kv23.5:3.7_0": "cakkhudhātu …",
  "kv23.5:3.8_0": "dhammadhātu …",
  "kv23.5:3.9_0": "cakkhundriyaṁ …pe…",
  "kv23.5:3.10_0": "aññātāvindriyaṁ aparinipphannanti?",
  "kv23.5:3.11_0": "Āmantā.",
  "kv23.5:3.12_0": "Aññātāvindriyaṁ na aniccaṁ …pe…",
  "kv23.5:3.13_0": "na vipariṇāmadhammanti?",
  "kv23.5:3.14_0": "Na hevaṁ vattabbe …pe…",
  "kv23.5:3.15_0": "nanu aññātāvindriyaṁ aniccaṁ saṅkhataṁ …pe… vipariṇāmadhammanti?",
  "kv23.5:3.16_0": "Āmantā.",
  "kv23.5:3.17_0": "Hañci aññātāvindriyaṁ aniccaṁ saṅkhataṁ paṭiccasamuppannaṁ khayadhammaṁ vayadhammaṁ virāgadhammaṁ nirodhadhammaṁ vipariṇāmadhammaṁ,",
  "kv23.5:3.17_1": "no ca vata re vattabbe—",
  "kv23.5:3.18_0": "“aññātāvindriyaṁ aparinipphannan”ti.",
  "kv23.5:4.1_0": "Dukkhaññeva parinipphannanti?",
  "kv23.5:4.2_0": "Āmantā.",
  "kv23.5:4.3_0": "Nanu yadaniccaṁ taṁ dukkhaṁ vuttaṁ bhagavatā—",
  "kv23.5:4.4_0": "“aññātāvindriyaṁ aniccan”ti?",
  "kv23.5:4.5_0": "Āmantā.",
  "kv23.5:4.6_0": "Hañci yadaniccaṁ taṁ dukkhaṁ vuttaṁ bhagavatā—",
  "kv23.5:4.7_0": "“aññātāvindriyaṁ aniccaṁ”, no ca vata re vattabbe—",
  "kv23.5:4.8_0": "“dukkhaññeva parinipphannan”ti.",
  "kv23.5:4.9_0": "Aparinipphannakathā niṭṭhitā.",
  "kv23.5:4.10_0": "Tevīsatimo vaggo.",
  "kv23.5:4.11_0": "Tassuddānaṁ",
  "kv23.5:5.1_0": "Ekādhippāyena methuno dhammo paṭisevitabbo,",
  "kv23.5:5.1_1": "arahantānaṁ vaṇṇena amanussā methunaṁ dhammaṁ paṭisevanti,",
  "kv23.5:5.1_2": "bodhisatto issariyakāmakārikāhetu vinipātaṁ gacchati,",
  "kv23.5:5.1_3": "gabbhaseyyaṁ okkamati,",
  "kv23.5:5.1_4": "dukkarakārikaṁ akāsi,",
  "kv23.5:5.1_5": "aparantapaṁ akāsi,",
  "kv23.5:5.1_6": "aññaṁ satthāraṁ uddisi,",
  "kv23.5:5.1_7": "atthi na rāgo rāgapatirūpako,",
  "kv23.5:5.1_8": "atthi na doso dosapatirūpako,",
  "kv23.5:5.1_9": "atthi na moho mohapatirūpako atthi na kileso kilesapatirūpako,",
  "kv23.5:5.1_10": "rūpaṁ aparinipphannaṁ aññātāvindriyaṁ aparinipphannanti.",
  "kv23.5:5.2_0": "Khuddako aḍḍhapaṇṇāsako.",
  "kv23.5:5.3_0": "Tassuddānaṁ",
  "kv23.5:6.1_0": "Navaṁ, nibbuti, ekādhippāyoti.",
  "kv23.5:6.2_0": "Paṇṇāsakuddānaṁ",
  "kv23.5:7.1_0": "Mahāniyāmo anusayā,",
  "kv23.5:7.2_0": "niggaho khuddakapañcamo;",
  "kv23.5:7.3_0": "Parappavādamaddanā,",
  "kv23.5:7.4_0": "suttamūlasamāhitā;",
  "kv23.5:7.5_0": "Ujjotanā satthusamaye,",
  "kv23.5:7.6_0": "kathāvatthupakaraṇeti.",
  "kv23.5:7.7_0": "Pañcattiṁsabhāṇavāraṁ",
  "kv23.5:7.8_0": "kathāvatthupakaraṇaṁ niṭṭhitaṁ."
}