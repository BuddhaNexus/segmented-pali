{
  "mil3.5.7:0.1_0": "Milindapañha",
  "mil3.5.7:0.2_0": "Buddhavagga",
  "mil3.5.7:0.3_0": "7. Aññakāyasaṅkamanapañha",
  "mil3.5.7:1.1_0": "Rājā āha—",
  "mil3.5.7:1.2_0": "“bhante nāgasena,",
  "mil3.5.7:1.2_1": "atthi koci satto yo imamhā kāyā aññaṁ kāyaṁ saṅkamatī”ti?",
  "mil3.5.7:1.3_0": "“Na hi, mahārājā”ti.",
  "mil3.5.7:1.4_0": "“Yadi, bhante nāgasena,",
  "mil3.5.7:1.4_1": "imamhā kāyā aññaṁ kāyaṁ saṅkamanto natthi,",
  "mil3.5.7:1.4_2": "nanu mutto bhavissati pāpakehi kammehī”ti?",
  "mil3.5.7:1.5_0": "“Āma, mahārāja, yadi na paṭisandaheyya,",
  "mil3.5.7:1.5_1": "mutto bhavissati pāpakehi kammehīti, yasmā ca kho, mahārāja, paṭisandahati,",
  "mil3.5.7:1.5_2": "tasmā na parimutto pāpakehi kammehī”ti.",
  "mil3.5.7:2.1_0": "“Opammaṁ karohī”ti.",
  "mil3.5.7:2.2_0": "“Yathā, mahārāja,",
  "mil3.5.7:2.2_1": "kocideva puriso aññatarassa purisassa ambaṁ avahareyya,",
  "mil3.5.7:2.2_2": "kiṁ so daṇḍappatto bhaveyyā”ti?",
  "mil3.5.7:2.3_0": "“Āma, bhante, daṇḍappatto bhaveyyā”ti.",
  "mil3.5.7:2.4_0": "“Na kho so, mahārāja, tāni ambāni avahari, yāni tena ropitāni,",
  "mil3.5.7:2.4_1": "kasmā daṇḍappatto bhaveyyā”ti?",
  "mil3.5.7:2.5_0": "“Tāni, bhante, ambāni nissāya jātāni,",
  "mil3.5.7:2.5_1": "tasmā daṇḍappatto bhaveyyā”ti.",
  "mil3.5.7:2.6_0": "“Evameva kho, mahārāja,",
  "mil3.5.7:2.6_1": "iminā nāmarūpena kammaṁ karoti sobhanaṁ vā asobhanaṁ vā,",
  "mil3.5.7:2.6_2": "tena kammena aññaṁ nāmarūpaṁ paṭisandahati,",
  "mil3.5.7:2.6_3": "tasmā na parimutto pāpakehi kammehī”ti.",
  "mil3.5.7:3.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.5.7:4.1_0": "Aññakāyasaṅkamanapañho sattamo."
}