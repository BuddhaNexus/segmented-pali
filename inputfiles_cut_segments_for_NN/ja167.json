{
  "ja167:0.1_0": "Jātaka",
  "ja167:0.2_0": "Dukanipāta",
  "ja167:0.3_0": "Santhavavagga",
  "ja167:0.4_0": "7. Samiddhijātaka",
  "ja167:1.1_0": "“Abhutvā bhikkhasi bhikkhu,",
  "ja167:1.2_0": "Na hi bhutvāna bhikkhasi;",
  "ja167:1.3_0": "Bhutvāna bhikkhu bhikkhassu,",
  "ja167:1.4_0": "Mā taṁ kālo upaccagā.",
  "ja167:2.1_0": "Kālaṁ vohaṁ na jānāmi,",
  "ja167:2.2_0": "channo kālo na dissati;",
  "ja167:2.3_0": "Tasmā abhutvā bhikkhāmi,",
  "ja167:2.4_0": "mā maṁ kālo upaccagā”ti.",
  "ja167:3.1_0": "Samiddhijātakaṁ sattamaṁ."
}