{
  "mil3.7.15:0.1_0": "Milindapañha",
  "mil3.7.15:0.2_0": "Arūpadhammavavatthānavagga",
  "mil3.7.15:0.3_0": "15. Viññāṇanānatthapañha",
  "mil3.7.15:1.1_0": "Rājā āha—",
  "mil3.7.15:1.2_0": "“bhante nāgasena,",
  "mil3.7.15:1.2_1": "‘viññāṇan’ti vā ‘paññā’ti vā ‘bhūtasmiṁ jīvo’ti vā ime dhammā nānatthā ceva nānābyañjanā ca,",
  "mil3.7.15:1.2_2": "udāhu ekatthā byañjanameva nānan”ti?",
  "mil3.7.15:1.3_0": "“Vijānanalakkhaṇaṁ, mahārāja, viññāṇaṁ, pajānanalakkhaṇā paññā, bhūtasmiṁ jīvo nupalabbhatī”ti.",
  "mil3.7.15:1.4_0": "“Yadi jīvo nupalabbhati,",
  "mil3.7.15:1.4_1": "atha ko carahi cakkhunā rūpaṁ passati,",
  "mil3.7.15:1.4_2": "sotena saddaṁ suṇāti,",
  "mil3.7.15:1.4_3": "ghānena gandhaṁ ghāyati,",
  "mil3.7.15:1.4_4": "jivhāya rasaṁ sāyati,",
  "mil3.7.15:1.4_5": "kāyena phoṭṭhabbaṁ phusati,",
  "mil3.7.15:1.4_6": "manasā dhammaṁ vijānātī”ti?",
  "mil3.7.15:1.5_0": "Thero āha—",
  "mil3.7.15:1.6_0": "“yadi jīvo cakkhunā rūpaṁ passati …pe…",
  "mil3.7.15:1.6_1": "manasā dhammaṁ vijānāti,",
  "mil3.7.15:1.6_2": "so jīvo cakkhudvāresu uppāṭitesu mahantena ākāsena bahimukho suṭṭhutaraṁ rūpaṁ passeyya,",
  "mil3.7.15:1.6_3": "sotesu uppāṭitesu,",
  "mil3.7.15:1.6_4": "ghāne uppāṭite,",
  "mil3.7.15:1.6_5": "jivhāya uppāṭitāya,",
  "mil3.7.15:1.6_6": "kāye uppāṭite mahantena ākāsena suṭṭhutaraṁ saddaṁ suṇeyya,",
  "mil3.7.15:1.6_7": "gandhaṁ ghāyeyya,",
  "mil3.7.15:1.6_8": "rasaṁ sāyeyya,",
  "mil3.7.15:1.6_9": "phoṭṭhabbaṁ phuseyyā”ti?",
  "mil3.7.15:1.7_0": "“Na hi, bhante”ti.",
  "mil3.7.15:1.8_0": "“Tena hi, mahārāja, bhūtasmiṁ jīvo nupalabbhatī”ti.",
  "mil3.7.15:2.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.7.15:3.1_0": "Viññāṇanānatthapañho pannarasamo."
}