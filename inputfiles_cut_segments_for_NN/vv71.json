{
  "vv71:0.1_0": "Vimānavatthu",
  "vv71:0.2_0": "Purisavimāna",
  "vv71:0.3_0": "Pāyāsivagga",
  "vv71:0.4_0": "7. Yavapālakavimānavatthu",
  "vv71:1.1_0": "“Uccamidaṁ maṇithūṇaṁ vimānaṁ,",
  "vv71:1.2_0": "…pe…",
  "vv71:1.3_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv71:2.1_0": "So devaputto attamano,",
  "vv71:2.2_0": "…pe…",
  "vv71:2.3_0": "yassa kammassidaṁ phalaṁ.",
  "vv71:3.1_0": "“Ahaṁ manussesu manussabhūto,",
  "vv71:3.2_0": "Ahosiṁ yavapālako;",
  "vv71:3.3_0": "Addasaṁ virajaṁ bhikkhuṁ,",
  "vv71:3.4_0": "Vippasannamanāvilaṁ.",
  "vv71:4.1_0": "Tassa adāsahaṁ bhāgaṁ,",
  "vv71:4.2_0": "pasanno sehi pāṇibhi;",
  "vv71:4.3_0": "Kummāsapiṇḍaṁ datvāna,",
  "vv71:4.4_0": "modāmi nandane vane.",
  "vv71:5.1_0": "Tena metādiso vaṇṇo,",
  "vv71:5.2_0": "…pe…",
  "vv71:5.3_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv71:6.1_0": "Yavapālakavimānaṁ sattamaṁ."
}