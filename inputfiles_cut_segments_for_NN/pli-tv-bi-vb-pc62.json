{
  "pli-tv-bi-vb-pc62:0.1_0": "Theravāda Vinaya",
  "pli-tv-bi-vb-pc62:0.2_0": "Bhikkhunivibhaṅga",
  "pli-tv-bi-vb-pc62:0.3_0": "Pācittiyakaṇḍa",
  "pli-tv-bi-vb-pc62:0.4_0": "Gabbhinivagga",
  "pli-tv-bi-vb-pc62:0.5_0": "62. Pāyantisikkhāpada",
  "pli-tv-bi-vb-pc62:1.1_0": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bi-vb-pc62:1.2_0": "Tena kho pana samayena bhikkhuniyo pāyantiṁ vuṭṭhāpenti.",
  "pli-tv-bi-vb-pc62:1.3_0": "Sā piṇḍāya carati.",
  "pli-tv-bi-vb-pc62:1.4_0": "Manussā evamāhaṁsu—",
  "pli-tv-bi-vb-pc62:1.5_0": "“dethāyyāya bhikkhaṁ, sadutiyikā ayyā”ti.",
  "pli-tv-bi-vb-pc62:1.6_0": "Manussā ujjhāyanti khiyyanti vipācenti—",
  "pli-tv-bi-vb-pc62:1.7_0": "“kathañhi nāma bhikkhuniyo pāyantiṁ vuṭṭhāpessantī”ti.",
  "pli-tv-bi-vb-pc62:1.8_0": "Assosuṁ kho bhikkhuniyo tesaṁ manussānaṁ ujjhāyantānaṁ khiyyantānaṁ vipācentānaṁ.",
  "pli-tv-bi-vb-pc62:1.9_0": "Yā tā bhikkhuniyo appicchā …pe… tā ujjhāyanti khiyyanti vipācenti—",
  "pli-tv-bi-vb-pc62:1.10_0": "“kathañhi nāma bhikkhuniyo pāyantiṁ vuṭṭhāpessantī”ti …pe…",
  "pli-tv-bi-vb-pc62:1.11_0": "“saccaṁ kira, bhikkhave, bhikkhuniyo pāyantiṁ vuṭṭhāpentī”ti?",
  "pli-tv-bi-vb-pc62:1.12_0": "“Saccaṁ, bhagavā”ti.",
  "pli-tv-bi-vb-pc62:1.13_0": "Vigarahi buddho bhagavā …pe…",
  "pli-tv-bi-vb-pc62:1.14_0": "kathañhi nāma, bhikkhave, bhikkhuniyo pāyantiṁ vuṭṭhāpessanti.",
  "pli-tv-bi-vb-pc62:1.15_0": "Netaṁ, bhikkhave, appasannānaṁ vā pasādāya …pe…",
  "pli-tv-bi-vb-pc62:1.16_0": "evañca pana, bhikkhave, bhikkhuniyo imaṁ sikkhāpadaṁ uddisantu—",
  "pli-tv-bi-vb-pc62:1.17.1_0": "“Yā pana bhikkhunī pāyantiṁ vuṭṭhāpeyya, pācittiyan”ti.",
  "pli-tv-bi-vb-pc62:2.1_0": "Yā panāti",
  "pli-tv-bi-vb-pc62:2.2_0": "yā yādisā …pe…",
  "pli-tv-bi-vb-pc62:2.3_0": "bhikkhunīti",
  "pli-tv-bi-vb-pc62:2.4_0": "…pe… ayaṁ imasmiṁ atthe adhippetā bhikkhunīti.",
  "pli-tv-bi-vb-pc62:2.5_0": "Pāyantī nāma",
  "pli-tv-bi-vb-pc62:2.6_0": "mātā vā hoti dhāti vā.",
  "pli-tv-bi-vb-pc62:2.7_0": "Vuṭṭhāpeyyāti",
  "pli-tv-bi-vb-pc62:2.8_0": "upasampādeyya.",
  "pli-tv-bi-vb-pc62:2.9_0": "“Vuṭṭhāpessāmī”ti gaṇaṁ vā ācariniṁ vā pattaṁ vā cīvaraṁ vā pariyesati,",
  "pli-tv-bi-vb-pc62:2.9_1": "sīmaṁ vā sammannati,",
  "pli-tv-bi-vb-pc62:2.9_2": "āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc62:2.10_0": "Ñattiyā dukkaṭaṁ.",
  "pli-tv-bi-vb-pc62:2.11_0": "Dvīhi kammavācāhi dukkaṭā.",
  "pli-tv-bi-vb-pc62:2.12_0": "Kammavācāpariyosāne upajjhāyāya āpatti pācittiyassa.",
  "pli-tv-bi-vb-pc62:2.13_0": "Gaṇassa ca ācariniyā ca āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc62:2.14.1_0": "Pāyantiyā pāyantisaññā vuṭṭhāpeti, āpatti pācittiyassa.",
  "pli-tv-bi-vb-pc62:2.15_0": "Pāyantiyā vematikā vuṭṭhāpeti, āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc62:2.16_0": "Pāyantiyā apāyantisaññā vuṭṭhāpeti, anāpatti.",
  "pli-tv-bi-vb-pc62:2.17_0": "Apāyantiyā pāyantisaññā, āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc62:2.18_0": "Apāyantiyā vematikā, āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc62:2.19_0": "Apāyantiyā apāyantisaññā, anāpatti.",
  "pli-tv-bi-vb-pc62:2.20.1_0": "Anāpatti—",
  "pli-tv-bi-vb-pc62:2.21_0": "pāyantiṁ apāyantisaññā vuṭṭhāpeti,",
  "pli-tv-bi-vb-pc62:2.22_0": "apāyantiṁ apāyantisaññā vuṭṭhāpeti,",
  "pli-tv-bi-vb-pc62:2.23_0": "ummattikāya,",
  "pli-tv-bi-vb-pc62:2.24_0": "ādikammikāyāti.",
  "pli-tv-bi-vb-pc62:2.25_0": "Dutiyasikkhāpadaṁ niṭṭhitaṁ."
}