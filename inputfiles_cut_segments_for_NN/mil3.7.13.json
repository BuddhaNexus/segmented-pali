{
  "mil3.7.13:0.1_0": "Milindapañha",
  "mil3.7.13:0.2_0": "Arūpadhammavavatthānavagga",
  "mil3.7.13:0.3_0": "13. Samuddaekarasapañha",
  "mil3.7.13:1.1_0": "Rājā āha—",
  "mil3.7.13:1.2_0": "“bhante nāgasena,",
  "mil3.7.13:1.2_1": "kena kāraṇena samuddo ekaraso loṇaraso”ti?",
  "mil3.7.13:1.3_0": "“Cirasaṇṭhitattā kho, mahārāja,",
  "mil3.7.13:1.3_1": "udakassa samuddo ekaraso loṇaraso”ti.",
  "mil3.7.13:2.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.7.13:3.1_0": "Samuddaekarasapañho terasamo."
}