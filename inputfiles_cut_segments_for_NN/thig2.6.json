{
  "thig2.6:0.1_0": "Therīgāthā",
  "thig2.6:0.2_0": "Dukanipāta",
  "thig2.6:0.3_0": "6. Mettikātherīgāthā",
  "thig2.6:1.1_0": "“Kiñcāpi khomhi dukkhitā,",
  "thig2.6:1.2_0": "dubbalā gatayobbanā;",
  "thig2.6:1.3_0": "Daṇḍamolubbha gacchāmi,",
  "thig2.6:1.4_0": "pabbataṁ abhirūhiya.",
  "thig2.6:2.1_0": "Nikkhipitvāna saṅghāṭiṁ,",
  "thig2.6:2.2_0": "pattakañca nikujjiya;",
  "thig2.6:2.3_0": "Nisinnā camhi selamhi,",
  "thig2.6:2.4_0": "atha cittaṁ vimucci me;",
  "thig2.6:2.5_0": "Tisso vijjā anuppattā,",
  "thig2.6:2.6_0": "kataṁ buddhassa sāsanan”ti.",
  "thig2.6:3.1_0": "… Mettikā therī …."
}