{
  "thag1.30:0.1_0": "Theragāthā",
  "thag1.30:0.2_0": "Ekakanipāta",
  "thag1.30:0.3_0": "Tatiyavagga",
  "thag1.30:0.4_0": "1.30. Uttiyattheragāthā",
  "thag1.30:1.1_0": "“Ābādhe me samuppanne,",
  "thag1.30:1.2_0": "sati me udapajjatha;",
  "thag1.30:1.3_0": "Ābādho me samuppanno,",
  "thag1.30:1.4_0": "kālo me nappamajjitun”ti.",
  "thag1.30:1.5_0": "…",
  "thag1.30:1.6_0": "Uttiyo thero ….",
  "thag1.30:1.7_0": "vaggo tatiyo.",
  "thag1.30:2.0_0": "Tassuddānaṁ",
  "thag1.30:2.1_0": "Nigrodho cittako thero,",
  "thag1.30:2.2_0": "gosālathero sugandho;",
  "thag1.30:2.3_0": "Nandiyo abhayo thero,",
  "thag1.30:2.4_0": "thero lomasakaṅgiyo;",
  "thag1.30:2.5_0": "Jambugāmikaputto ca,",
  "thag1.30:2.6_0": "hārito uttiyo isīti."
}