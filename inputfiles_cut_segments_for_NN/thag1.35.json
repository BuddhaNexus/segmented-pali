{
  "thag1.35:0.1_0": "Theragāthā",
  "thag1.35:0.2_0": "Ekakanipāta",
  "thag1.35:0.3_0": "Catutthavagga",
  "thag1.35:0.4_0": "1.35. Sāmaññakānittheragāthā",
  "thag1.35:1.1_0": "“Sukhaṁ sukhattho labhate tadācaraṁ,",
  "thag1.35:1.2_0": "Kittiñca pappoti yasassa vaḍḍhati;",
  "thag1.35:1.3_0": "Yo ariyamaṭṭhaṅgikamañjasaṁ ujuṁ,",
  "thag1.35:1.4_0": "Bhāveti maggaṁ amatassa pattiyā”ti.",
  "thag1.35:1.5_0": "… Sāmaññakānitthero …."
}