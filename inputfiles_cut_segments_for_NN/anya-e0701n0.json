{
  "anya-e0701n0:0_0": " Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e0701n0:1_0": "Cūḷaganthavaṁsapāḷi",
  "anya-e0701n0:2_0": "1. Piṭakattayaparicchedo",
  "anya-e0701n0:3_0": "Namassetvāna sambuddhaṁ, aggavaṁsavaraṁvaraṁ;",
  "anya-e0701n0:4_0": "Natvāna dhammaṁ buddhajaṁ, saṅghañcāpiniraṅgaṇaṁ.",
  "anya-e0701n0:5_0": "Ganthavaṁsampi nissāya, ganthavaṁsaṁ pakathissaṁ;",
  "anya-e0701n0:6_0": "Tipeṭakasamāhāraṁ, sādhunaṁ jaṅghadāsakaṁ.",
  "anya-e0701n0:7_0": "Vimatinodanamārambhaṁ, taṁ me suṇātha sādhavo;",
  "anya-e0701n0:8_0": "Sabbampi buddhavacanaṁ, vimutti ca sahetukaṁ.",
  "anya-e0701n0:9_0": "Hoti ekavidhaṁyeva, tividhaṁ piṭakena ca;",
  "anya-e0701n0:10_0": "Tañca sabbampi kevalaṁ, pañcavidhaṁ nikāyato.",
  "anya-e0701n0:11_0": "Aṅgato ca navavidhaṁ, dhammakkhandhagaṇanato;",
  "anya-e0701n0:12_0": "Caturāsīti sahassa, dhammakkhandhapabhedananti.",
  "anya-e0701n0:13_0": "Kathaṁ piṭakato piṭakañhi tividhaṁ hoti?",
  "anya-e0701n0:14_0": "Vinayapiṭakaṁ,",
  "anya-e0701n0:14_1": "abhidhammapiṭakaṁ suttantapiṭakanti.",
  "anya-e0701n0:14_2": "Tattha katamaṁ vinaya piṭakaṁ?",
  "anya-e0701n0:14_3": "Pārājikakaṇḍaṁ,",
  "anya-e0701n0:14_4": "pācittiyakaṇḍaṁ,",
  "anya-e0701n0:14_5": "mahāvaggakaṇḍaṁ,",
  "anya-e0701n0:14_6": "cullavaggakaṇḍaṁ,",
  "anya-e0701n0:14_7": "parivārakaṇḍanti.",
  "anya-e0701n0:14_8": "Imāni kaṇḍāni vinayapiṭakaṁ nāma.",
  "anya-e0701n0:15_0": "Katamaṁ abhidhammapiṭakaṁ?",
  "anya-e0701n0:15_1": "Dhammasaṅgaṇī-pakaraṇaṁ,",
  "anya-e0701n0:15_2": "vibhaṅga-pakaraṇaṁ,",
  "anya-e0701n0:15_3": "dhātukathā-pakaraṇaṁ,",
  "anya-e0701n0:15_4": "puggalapaññatti-pakaraṇaṁ,",
  "anya-e0701n0:15_5": "kathāvatthu-pakaraṇaṁ,",
  "anya-e0701n0:15_6": "yamaka-pakaraṇaṁ,",
  "anya-e0701n0:15_7": "paṭṭhāna-pakaraṇanti.",
  "anya-e0701n0:15_8": "Imāni satta pakaraṇāni abhidhammapiṭakaṁ nāma.",
  "anya-e0701n0:15_9": "Katamaṁ suttantapiṭakaṁ?",
  "anya-e0701n0:15_10": "Sīlakkhandhavaggādikaṁ,",
  "anya-e0701n0:15_11": "avasesaṁ buddhavacanaṁ suttantapiṭakaṁ nāma.",
  "anya-e0701n0:16_0": "Kathaṁ nikāyato?",
  "anya-e0701n0:16_1": "Nikāyā pañca vidhā honti.",
  "anya-e0701n0:16_2": "Dīghanikāyo,",
  "anya-e0701n0:16_3": "majjhimanikāyo,",
  "anya-e0701n0:16_4": "saṁyuttanikāyo,",
  "anya-e0701n0:16_5": "aṅguttaranikāyo,",
  "anya-e0701n0:16_6": "khuddakanikāyoti.",
  "anya-e0701n0:17_0": "Tattha katamo dīgha-nikāyo?",
  "anya-e0701n0:17_1": "Sīlakkhandhavaggo,",
  "anya-e0701n0:17_2": "mahāvaggo,",
  "anya-e0701n0:17_3": "pāthikavaggoti,",
  "anya-e0701n0:17_4": "ime tayo vaggā dīghanikāyo nāma.",
  "anya-e0701n0:17_5": "Imesu tīsu vaggesu,",
  "anya-e0701n0:17_6": "catutiṁsa vaggāni ca honti.",
  "anya-e0701n0:17_7": "[Catutiṁseva suttantā,",
  "anya-e0701n0:17_8": "sīlakkhandhavaggādikā,",
  "anya-e0701n0:17_9": "yassa bhavanti so yeva dīghanikāya nāma hoti.]",
  "anya-e0701n0:18_0": "Katamo majjhimanikāyo?",
  "anya-e0701n0:18_1": "Mūlapaṇṇāso,",
  "anya-e0701n0:18_2": "majjhimapaṇṇāso,",
  "anya-e0701n0:18_3": "uparipaṇṇāsoti,",
  "anya-e0701n0:18_4": "ime tayo paṇṇāsā majjhimanikāyo nāma.",
  "anya-e0701n0:18_5": "Imesu tīsu paṇṇāsesu dvepaññāsādhika-sutta-satāni honti [diyaḍḍhasatasuttantā,",
  "anya-e0701n0:18_6": "dvi suttaṁ yassa santiso,",
  "anya-e0701n0:18_7": "majjhimanikāyo nāmo mūlapaṇṇāsamādi hoti.]",
  "anya-e0701n0:19_0": "Katamo saṁyuttanikāyo?",
  "anya-e0701n0:19_1": "Sagāthāvaggo,",
  "anya-e0701n0:19_2": "nidānavaggo,",
  "anya-e0701n0:19_3": "khandhakavaggo,",
  "anya-e0701n0:19_4": "saḷāyatanavaggo,",
  "anya-e0701n0:19_5": "mahāvaggoti,",
  "anya-e0701n0:19_6": "ime pañca vaggā saṁyuttanikāyo nāma.",
  "anya-e0701n0:19_7": "Imesu pañcasu vaggesu dvāsaṭṭhisuttasattasatādhikasatta-suttasahassāni honti.",
  "anya-e0701n0:19_8": "[Dvāsaṭṭhi-satta-satāni,",
  "anya-e0701n0:19_9": "sattasahassakāni ca.] Suttāni yassa honti so,",
  "anya-e0701n0:19_10": "sagāthādikavaggako,",
  "anya-e0701n0:19_11": "saṁyuttanikāyo nāmo veditabbo ca viññūnāti.",
  "anya-e0701n0:20_0": "Katamo aṅguttaranikāyo?",
  "anya-e0701n0:20_1": "Ekkanipāto,",
  "anya-e0701n0:20_2": "dukkanipāto,",
  "anya-e0701n0:20_3": "tikkanipāto,",
  "anya-e0701n0:20_4": "catukkanipāto,",
  "anya-e0701n0:20_5": "pañcakanipāto,",
  "anya-e0701n0:20_6": "chakkanipāto,",
  "anya-e0701n0:20_7": "sattakanipāto,",
  "anya-e0701n0:20_8": "aṭṭhakanipāto,",
  "anya-e0701n0:20_9": "navakanipāto,",
  "anya-e0701n0:20_10": "dasakanipāto,",
  "anya-e0701n0:20_11": "ekādasanipātoti,",
  "anya-e0701n0:20_12": "ime ekādasa nipātā aṅguttaranikāyo nāma.",
  "anya-e0701n0:20_13": "Imesu ekādasa nipātesu satta-paññāsa-pañca-satādhikanava-sutta-sahassāni honti.",
  "anya-e0701n0:20_14": "[Navasuttasahassāni,",
  "anya-e0701n0:20_15": "pañcasatamattāni ca,",
  "anya-e0701n0:20_16": "sattapaññāsādhikāni,",
  "anya-e0701n0:20_17": "suttāni yassa honti so,",
  "anya-e0701n0:20_18": "aṅguttaranikāyoti,",
  "anya-e0701n0:20_19": "ekkanipātakādikoti.]",
  "anya-e0701n0:21_0": "Katamo khuddakanikāyo?",
  "anya-e0701n0:21_1": "Khuddakapāṭho,",
  "anya-e0701n0:21_2": "dhammapadaṁ, udānaṁ,",
  "anya-e0701n0:21_3": "itivuttakaṁ,",
  "anya-e0701n0:21_4": "suttanipāto,",
  "anya-e0701n0:21_5": "vimānavatthu,",
  "anya-e0701n0:21_6": "petavatthu,",
  "anya-e0701n0:21_7": "therakathā,",
  "anya-e0701n0:21_8": "therīkathā, jātakaṁ,",
  "anya-e0701n0:21_9": "mahāniddeso,",
  "anya-e0701n0:21_10": "paṭisambhidāmaggo, apadānaṁ,",
  "anya-e0701n0:21_11": "buddhavaṁso,",
  "anya-e0701n0:21_12": "cariyāpiṭakaṁ,",
  "anya-e0701n0:21_13": "vinayapiṭakaṁ,",
  "anya-e0701n0:21_14": "abhidhammapiṭakanti.",
  "anya-e0701n0:21_15": "Imesu sattarasasu ganthesu anekāni sutta-sahassāni honti.",
  "anya-e0701n0:21_16": "[Anekāni sutta-sahassāni,",
  "anya-e0701n0:21_17": "niddiṭṭhāni mahesinā,",
  "anya-e0701n0:21_18": "nikāye pañcame ime,",
  "anya-e0701n0:21_19": "khuddake iti visuteti.]",
  "anya-e0701n0:22_0": "Kathaṁ aṅgato aṅgahi nava vidhaṁ hoti? Suttaṁ, geyyaṁ,",
  "anya-e0701n0:22_1": "veyyākaraṇaṁ, gāthā, udānaṁ,",
  "anya-e0701n0:22_2": "itivuttakaṁ, jātakaṁ,",
  "anya-e0701n0:22_3": "abbhutadhammaṁ,",
  "anya-e0701n0:22_4": "vedallanti,",
  "anya-e0701n0:22_5": "navappabhedaṁ hoti.",
  "anya-e0701n0:22_6": "Tattha ubhato vibhaṅganiddesakhandhakaparivārā,",
  "anya-e0701n0:22_7": "suttanipāte,",
  "anya-e0701n0:22_8": "maṅgalasutta,",
  "anya-e0701n0:22_9": "ratanasutta,",
  "anya-e0701n0:22_10": "tuvaṭṭakasuttāni.",
  "anya-e0701n0:22_11": "Aññampi suttanāmakaṁ tathāgatavacanaṁ,",
  "anya-e0701n0:22_12": "suttanti veditabbaṁ.",
  "anya-e0701n0:22_13": "Sabbaṁ sagāthakaṁ geyyanti veditabbaṁ.",
  "anya-e0701n0:22_14": "Visesanasaṁyuttake sakalopi sagāthakavaggo,",
  "anya-e0701n0:22_15": "sakalaṁ abhidhammapiṭakaṁ nigāthakañca suttayañca aññampi aṭṭhahi aṅgehi asaṅgahitaṁ buddhavacanaṁ taṁ veyyākaraṇanti veditabbaṁ.",
  "anya-e0701n0:22_16": "Dhammapadaṁ,",
  "anya-e0701n0:22_17": "therakathā,",
  "anya-e0701n0:22_18": "therīkathā,",
  "anya-e0701n0:22_19": "suttanipāte,",
  "anya-e0701n0:22_20": "no suttanāmikā suddhikagāthā,",
  "anya-e0701n0:22_21": "gāthāti veditabbā.",
  "anya-e0701n0:22_22": "Somanassa ñāṇamayikagāthā paṭika-saṁyuttā dve asītisuttantā udānanti veditabbā.",
  "anya-e0701n0:22_23": "Vuttañhetaṁ bhagavatātiādinayappavattā dasuttarasatasuttantā,",
  "anya-e0701n0:22_24": "itivuttakanti veditabbā.",
  "anya-e0701n0:22_25": "Apaṇṇakajātakādīni paññāsādikāni.",
  "anya-e0701n0:22_26": "Pañcajātakasatāni,",
  "anya-e0701n0:22_27": "jātakanti veditabbaṁ.",
  "anya-e0701n0:22_28": "Cattāro me bhikkhave acchariyā abhūtadhammā,",
  "anya-e0701n0:22_29": "ānandetiādinayappavattā sabbepi acchariya abhūtadhammapaṭisaṁyuttā suttantā abhūtadhammanti veditabbā.",
  "anya-e0701n0:22_30": "Cullavedalla,",
  "anya-e0701n0:22_31": "mahāvedalla,",
  "anya-e0701n0:22_32": "sammādiṭṭhi,",
  "anya-e0701n0:22_33": "sakkapañha,",
  "anya-e0701n0:22_34": "saṅkhārabhājaniya,",
  "anya-e0701n0:22_35": "mahāpuṇṇamasuttantādayo sabbepi vedañca tuṭṭhiñca laddhā [puccha] laddhā pucchitasuttantā,",
  "anya-e0701n0:22_36": "vedallanti veditabbā.",
  "anya-e0701n0:22_37": "Katamāni caturāsīti dhammakkhandhasahassāni dujānāni,",
  "anya-e0701n0:22_38": "caturāsīti dhammakkhandhasahassāni sace vitthārena kathissaṁ atipapañco bhavissati.",
  "anya-e0701n0:22_39": "Tasmā naya vasena kathissāmi.",
  "anya-e0701n0:22_40": "Ekaṁ vatthu,",
  "anya-e0701n0:22_41": "eko dhammakkhandho,",
  "anya-e0701n0:22_42": "ekaṁ nidānaṁ eko dhammakkhandho,",
  "anya-e0701n0:22_43": "ekaṁ pañhā pucchantaṁ eko dhammakkhandho,",
  "anya-e0701n0:22_44": "ekaṁ pañhā visajjanaṁ eko dhammakkhandho,",
  "anya-e0701n0:22_45": "caturāsīti dhammakkhandhasahassāni kena bhāsitāni,",
  "anya-e0701n0:22_46": "kattha bhāsitāni,",
  "anya-e0701n0:22_47": "kadā bhāsitāni,",
  "anya-e0701n0:22_48": "kamārabbha bhāsitāni,",
  "anya-e0701n0:22_49": "kimatthaṁ bhāsitāni,",
  "anya-e0701n0:22_50": "kena dhāritāni,",
  "anya-e0701n0:22_51": "kenābhatāni,",
  "anya-e0701n0:22_52": "kimatthaṁ pariyāpuṇitabbāni.",
  "anya-e0701n0:22_53": "Tatrāyaṁ visajjanā,",
  "anya-e0701n0:22_54": "kena bhāsitānīti?",
  "anya-e0701n0:22_55": "Buddhānu buddhehi bhāsitāni.",
  "anya-e0701n0:22_56": "Kattha bhāsitānīti?",
  "anya-e0701n0:22_57": "Devesu ca mānussesu ca,",
  "anya-e0701n0:22_58": "bhāsitāni.",
  "anya-e0701n0:22_59": "Kadā bhāsitānīti?",
  "anya-e0701n0:22_60": "Bhagavato dharamānakāle ceva pacchimakāle ca bhāsitāni.",
  "anya-e0701n0:22_61": "Katamārabbha bhāsitānīti?",
  "anya-e0701n0:22_62": "Pañcavaggiyādike veneyya bandhave ārabbha bhāsitānīti.",
  "anya-e0701n0:22_63": "Kimatthaṁ bhāsitānīti?",
  "anya-e0701n0:22_64": "Tivajjañca avajjañca ñatvā vajjaṁ pahāya avajje paṭipattitvā nibbānapariyante.",
  "anya-e0701n0:22_65": "Diṭṭha-dhammikasamparāyikatthe sampāpuṇituṁ.",
  "anya-e0701n0:23_0": "Kena dhāritānīti?",
  "anya-e0701n0:23_1": "Anubuddhehi ceva sissānusissehi ca dhāritāni.",
  "anya-e0701n0:23_2": "Kenā bhatānīti?",
  "anya-e0701n0:23_3": "Ācariya paraṁparehi ābhatāni.",
  "anya-e0701n0:23_4": "Kimatthaṁ pariyāpuṇitabbānīti?",
  "anya-e0701n0:23_5": "Vajjañca avajjañca ñatvā vajjaṁ pahāya avajje paṭipattitvā nibbānapariyante diṭṭhadhammikasamparāyikatthe,",
  "anya-e0701n0:23_6": "saṁpāpuṇituṁ,",
  "anya-e0701n0:23_7": "yadevaṁ tāya nibbānapariyante diṭṭhadhammikasamparāyikatthe sādhikāni honti.",
  "anya-e0701n0:23_8": "Teva tattha kehi appamattena pariyāpuṇitabbāni dhāretabbāni dhāretabbāni vācetabbāni sajjhāyaṁ kātabbānīti",
  "anya-e0701n0:23_9": "[iti cullaganthavaṁse piṭakattaya dīpako nāma paṭhamo paricchedo.]"
}