{
  "mil7.2.8:0.1_0": "Milindapañha",
  "mil7.2.8:0.2_0": "Opammakathāpañha",
  "mil7.2.8:0.3_0": "Gadrabhavagga",
  "mil7.2.8:0.4_0": "8. Cāpaṅgapañha",
  "mil7.2.8:1.1_0": "“Bhante nāgasena,",
  "mil7.2.8:1.1_1": "‘cāpassa ekaṁ aṅgaṁ gahetabban’ti yaṁ vadesi,",
  "mil7.2.8:1.1_2": "katamaṁ taṁ ekaṁ aṅgaṁ gahetabban”ti?",
  "mil7.2.8:1.2_0": "“Yathā, mahārāja,",
  "mil7.2.8:1.2_1": "cāpo sutacchito namito yāvaggamūlaṁ samakameva anunamati nappaṭitthambhati;",
  "mil7.2.8:1.3_0": "evameva kho, mahārāja,",
  "mil7.2.8:1.3_1": "yoginā yogāvacarena theranavamajjhimasamakesu anunamitabbaṁ nappaṭipharitabbaṁ.",
  "mil7.2.8:1.4_0": "Idaṁ, mahārāja, cāpassa ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.2.8:1.5_0": "Bhāsitampetaṁ, mahārāja,",
  "mil7.2.8:1.5_1": "bhagavatā devātidevena vidhura (puṇṇaka) jātake—",
  "mil7.2.8:2.1_0": "‘Cāpovūnudaro dhīro,",
  "mil7.2.8:2.2_0": "vaṁso vāpi pakampaye;",
  "mil7.2.8:2.3_0": "Paṭilomaṁ na vatteyya,",
  "mil7.2.8:2.4_0": "sa rājavasatiṁ vase’”ti.",
  "mil7.2.8:3.1_0": "Cāpaṅgapañho aṭṭhamo."
}