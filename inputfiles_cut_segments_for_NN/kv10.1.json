{
  "kv10.1:0.1_0": "Kathāvatthu",
  "kv10.1:0.2_0": "Niyāmapaṇṇāsaka",
  "kv10.1:0.3_0": "Dasamavagga",
  "kv10.1:0.4_0": "Nirodhakathā",
  "kv10.1:1.1_0": "Upapattesiye pañcakkhandhe aniruddhe kiriyā pañcakkhandhā uppajjantīti?",
  "kv10.1:1.2_0": "Āmantā.",
  "kv10.1:1.3_0": "Dasannaṁ khandhānaṁ samodhānaṁ hoti,",
  "kv10.1:1.3_1": "dasa khandhā sammukhībhāvaṁ āgacchantīti?",
  "kv10.1:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv10.1:1.5_0": "dasannaṁ khandhānaṁ samodhānaṁ hoti,",
  "kv10.1:1.5_1": "dasa khandhā sammukhībhāvaṁ āgacchantīti?",
  "kv10.1:1.6_0": "Āmantā.",
  "kv10.1:1.6_1": "dvinnaṁ phassānaṁ …pe… dvinnaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv10.1:1.7_0": "Na hevaṁ vattabbe …pe….",
  "kv10.1:2.1_0": "Upapattesiye pañcakkhandhe aniruddhe kiriyā cattāro khandhā uppajjantīti?",
  "kv10.1:2.2_0": "Āmantā.",
  "kv10.1:2.3_0": "Navannaṁ khandhānaṁ samodhānaṁ hoti,",
  "kv10.1:2.3_1": "nava khandhā sammukhībhāvaṁ āgacchantīti?",
  "kv10.1:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv10.1:2.5_0": "navannaṁ khandhānaṁ samodhānaṁ hoti,",
  "kv10.1:2.5_1": "nava khandhā sammukhībhāvaṁ āgacchantīti?",
  "kv10.1:2.6_0": "Āmantā.",
  "kv10.1:2.6_1": "dvinnaṁ phassānaṁ …pe… dvinnaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv10.1:2.7_0": "Na hevaṁ vattabbe …pe….",
  "kv10.1:3.1_0": "Upapattesiye pañcakkhandhe aniruddhe kiriyāñāṇaṁ uppajjatīti?",
  "kv10.1:3.2_0": "Āmantā.",
  "kv10.1:3.3_0": "Channaṁ khandhānaṁ samodhānaṁ hoti,",
  "kv10.1:3.3_1": "cha khandhā sammukhībhāvaṁ āgacchantīti?",
  "kv10.1:3.4_0": "Na hevaṁ vattabbe …pe…",
  "kv10.1:3.5_0": "channaṁ khandhānaṁ samodhānaṁ hoti,",
  "kv10.1:3.5_1": "cha khandhā sammukhībhāvaṁ āgacchantīti?",
  "kv10.1:3.6_0": "Āmantā.",
  "kv10.1:3.6_1": "dvinnaṁ phassānaṁ …pe… dvinnaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv10.1:3.7_0": "Na hevaṁ vattabbe …pe….",
  "kv10.1:4.1_0": "Upapattesiye pañcakkhandhe niruddhe maggo uppajjatīti?",
  "kv10.1:4.2_0": "Āmantā.",
  "kv10.1:4.3_0": "Mato maggaṁ bhāveti, kālaṅkato maggaṁ bhāvetīti?",
  "kv10.1:4.4_0": "Na hevaṁ vattabbe …pe….",
  "kv10.1:4.5_0": "Nirodhakathā niṭṭhitā."
}