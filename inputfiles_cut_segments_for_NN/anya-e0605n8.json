{
  "anya-e0605n8:0_0": "Agāriyasampattidīpanīgāthā",
  "anya-e0605n8:1_0": "46.",
  "anya-e0605n8:2_0": " Kālakkamena cando va vaḍḍhanto vaḍḍhite kule,",
  "anya-e0605n8:3_0": "Puññodayenudento so bhāṇumā viya ambare.",
  "anya-e0605n8:4_0": "47.",
  "anya-e0605n8:5_0": " Siddhathako hi siddhattho laddhā deviṁ yasodharaṁ,",
  "anya-e0605n8:6_0": "Cattālīsasahassehi pūritthīhi purakkhito.",
  "anya-e0605n8:7_0": "48.",
  "anya-e0605n8:8_0": " Rammasurammasubhesu gharesu,",
  "anya-e0605n8:9_0": "Tiṇṇamutūnamanucchavikesu;",
  "anya-e0605n8:10_0": "Dibbasukhaṁ viya bhuñji sukhaṁ so,",
  "anya-e0605n8:11_0": "Acchariyabbhutarājavibhūtiṁ."
}