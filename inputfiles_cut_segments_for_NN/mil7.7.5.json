{
  "mil7.7.5:0.1_0": "Milindapañha",
  "mil7.7.5:0.2_0": "Opammakathāpañha",
  "mil7.7.5:0.3_0": "Makkaṭakavagga",
  "mil7.7.5:0.4_0": "5. Rukkhaṅgapañha",
  "mil7.7.5:1.1_0": "“Bhante nāgasena,",
  "mil7.7.5:1.1_1": "‘rukkhassa tīṇi aṅgāni gahetabbānī’ti yaṁ vadesi,",
  "mil7.7.5:1.1_2": "katamāni tāni tīṇi aṅgāni gahetabbānī”ti?",
  "mil7.7.5:1.2_0": "“Yathā, mahārāja, rukkho nāma pupphaphaladharo;",
  "mil7.7.5:1.3_0": "evameva kho, mahārāja,",
  "mil7.7.5:1.3_1": "yoginā yogāvacarena vimuttipupphasāmaññaphaladhārinā bhavitabbaṁ.",
  "mil7.7.5:1.4_0": "Idaṁ, mahārāja,",
  "mil7.7.5:1.4_1": "rukkhassa paṭhamaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.7.5:2.1_0": "Puna caparaṁ, mahārāja,",
  "mil7.7.5:2.1_1": "rukkho upagatānamanuppaviṭṭhānaṁ janānaṁ chāyaṁ deti;",
  "mil7.7.5:2.2_0": "evameva kho, mahārāja,",
  "mil7.7.5:2.2_1": "yoginā yogāvacarena upagatānamanuppaviṭṭhānaṁ puggalānaṁ āmisappaṭisandhārena vā dhammappaṭisanthārena vā paṭisantharitabbaṁ.",
  "mil7.7.5:2.3_0": "Idaṁ, mahārāja,",
  "mil7.7.5:2.3_1": "rukkhassa dutiyaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.7.5:3.1_0": "Puna caparaṁ, mahārāja,",
  "mil7.7.5:3.1_1": "rukkho chāyāvemattaṁ na karoti;",
  "mil7.7.5:3.2_0": "evameva kho, mahārāja,",
  "mil7.7.5:3.2_1": "yoginā yogāvacarena sabbasattesu vemattatā na kātabbā,",
  "mil7.7.5:3.2_2": "coravadhakapaccatthikesupi attanipi samasamā mettābhāvanā kātabbā,",
  "mil7.7.5:3.2_3": "‘kinti ime sattā averā abyāpajjā anīghā sukhī attānaṁ parihareyyun’ti.",
  "mil7.7.5:3.3_0": "Idaṁ, mahārāja,",
  "mil7.7.5:3.3_1": "rukkhassa tatiyaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.7.5:3.4_0": "Bhāsitampetaṁ, mahārāja,",
  "mil7.7.5:3.4_1": "therena sāriputtena dhammasenāpatinā—",
  "mil7.7.5:4.1_0": "‘Vadhake devadattamhi,",
  "mil7.7.5:4.2_0": "core aṅgulimālake;",
  "mil7.7.5:4.3_0": "Dhanapāle rāhule ca,",
  "mil7.7.5:4.4_0": "sabbattha samako munī’”ti.",
  "mil7.7.5:5.1_0": "Rukkhaṅgapañho pañcamo."
}