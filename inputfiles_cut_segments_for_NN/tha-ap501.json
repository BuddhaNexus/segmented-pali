{
  "tha-ap501:0.1_0": "Therāpadāna",
  "tha-ap501:0.2_0": "Kiṅkaṇipupphavagga",
  "tha-ap501:0.3_0": "9 Naḷakuṭidāyakattheraapadāna",
  "tha-ap501:1.1_0": "“Himavantassāvidūre,",
  "tha-ap501:1.2_0": "hārito nāma pabbato;",
  "tha-ap501:1.3_0": "Sayambhū nārado nāma,",
  "tha-ap501:1.4_0": "rukkhamūle vasī tadā.",
  "tha-ap501:2.1_0": "Naḷāgāraṁ karitvāna,",
  "tha-ap501:2.2_0": "tiṇena chādayiṁ ahaṁ;",
  "tha-ap501:2.3_0": "Caṅkamaṁ sodhayitvāna,",
  "tha-ap501:2.4_0": "sayambhussa adāsahaṁ.",
  "tha-ap501:3.1_0": "Tena kammena sukatena,",
  "tha-ap501:3.2_0": "cetanāpaṇidhīhi ca;",
  "tha-ap501:3.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "tha-ap501:3.4_0": "tāvatiṁsamagacchahaṁ.",
  "tha-ap501:4.1_0": "Tattha me sukataṁ byamhaṁ,",
  "tha-ap501:4.2_0": "naḷakuṭikanimmitaṁ;",
  "tha-ap501:4.3_0": "Saṭṭhiyojanamubbedhaṁ,",
  "tha-ap501:4.4_0": "tiṁsayojanavitthataṁ.",
  "tha-ap501:5.1_0": "Catuddasesu kappesu,",
  "tha-ap501:5.2_0": "devaloke ramiṁ ahaṁ;",
  "tha-ap501:5.3_0": "Ekasattatikkhattuñca,",
  "tha-ap501:5.4_0": "devarajjamakārayiṁ.",
  "tha-ap501:6.1_0": "Catutiṁsatikkhattuñca,",
  "tha-ap501:6.2_0": "cakkavattī ahosahaṁ;",
  "tha-ap501:6.3_0": "Padesarajjaṁ vipulaṁ,",
  "tha-ap501:6.4_0": "gaṇanāto asaṅkhiyaṁ.",
  "tha-ap501:7.1_0": "Dhammapāsādamāruyha,",
  "tha-ap501:7.2_0": "sabbākāravarūpamaṁ;",
  "tha-ap501:7.3_0": "Yadicchakāhaṁ vihare,",
  "tha-ap501:7.4_0": "sakyaputtassa sāsane.",
  "tha-ap501:8.1_0": "Ekatiṁse ito kappe,",
  "tha-ap501:8.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap501:8.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap501:8.4_0": "naḷakuṭiyidaṁ phalaṁ.",
  "tha-ap501:9.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap501:9.2_0": "…pe…",
  "tha-ap501:9.3_0": "viharāmi anāsavo.",
  "tha-ap501:10.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap501:10.2_0": "…pe…",
  "tha-ap501:10.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap501:11.1_0": "Paṭisambhidā catasso,",
  "tha-ap501:11.2_0": "…pe…",
  "tha-ap501:11.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap501:12.1_0": "Itthaṁ sudaṁ āyasmā naḷakuṭidāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap501:13.1_0": "Naḷakuṭidāyakattherassāpadānaṁ navamaṁ."
}