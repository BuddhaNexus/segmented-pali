{
  "pli-tv-bi-vb-pc74:0.1_0": "Theravāda Vinaya",
  "pli-tv-bi-vb-pc74:0.2_0": "Bhikkhunivibhaṅga",
  "pli-tv-bi-vb-pc74:0.3_0": "Pācittiyakaṇḍa",
  "pli-tv-bi-vb-pc74:0.4_0": "Kumāribhūtavagga",
  "pli-tv-bi-vb-pc74:0.5_0": "74. Ūnadvādasavassasikkhāpada",
  "pli-tv-bi-vb-pc74:1.1_0": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bi-vb-pc74:1.2_0": "Tena kho pana samayena bhikkhuniyo ūnadvādasavassā vuṭṭhāpenti.",
  "pli-tv-bi-vb-pc74:1.3_0": "Tā bālā honti abyattā na jānanti kappiyaṁ vā akappiyaṁ vā.",
  "pli-tv-bi-vb-pc74:1.4_0": "Saddhivihāriniyopi bālā honti abyattā;",
  "pli-tv-bi-vb-pc74:1.5_0": "na jānanti kappiyaṁ vā akappiyaṁ vā.",
  "pli-tv-bi-vb-pc74:1.6_0": "Yā tā bhikkhuniyo appicchā …pe… tā ujjhāyanti khiyyanti vipācenti—",
  "pli-tv-bi-vb-pc74:1.7_0": "“kathañhi nāma bhikkhuniyo ūnadvādasavassā vuṭṭhāpessantī”ti …pe…",
  "pli-tv-bi-vb-pc74:1.8_0": "“saccaṁ kira, bhikkhave,",
  "pli-tv-bi-vb-pc74:1.8_1": "bhikkhuniyo ūnadvādasavassā vuṭṭhāpentī”ti?",
  "pli-tv-bi-vb-pc74:1.9_0": "“Saccaṁ, bhagavā”ti.",
  "pli-tv-bi-vb-pc74:1.10_0": "Vigarahi buddho bhagavā …pe…",
  "pli-tv-bi-vb-pc74:1.11_0": "kathañhi nāma, bhikkhave,",
  "pli-tv-bi-vb-pc74:1.11_1": "bhikkhuniyo ūnadvādasavassā vuṭṭhāpessanti.",
  "pli-tv-bi-vb-pc74:1.12_0": "Netaṁ, bhikkhave, appasannānaṁ vā pasādāya …pe…",
  "pli-tv-bi-vb-pc74:1.13_0": "evañca pana, bhikkhave,",
  "pli-tv-bi-vb-pc74:1.13_1": "bhikkhuniyo imaṁ sikkhāpadaṁ uddisantu—",
  "pli-tv-bi-vb-pc74:1.14.1_0": "“Yā pana bhikkhunī ūnadvādasavassā vuṭṭhāpeyya, pācittiyan”ti.",
  "pli-tv-bi-vb-pc74:2.1.1_0": "Yā panāti",
  "pli-tv-bi-vb-pc74:2.1.2_0": "yā yādisā …pe…",
  "pli-tv-bi-vb-pc74:2.1.3_0": "bhikkhunīti",
  "pli-tv-bi-vb-pc74:2.1.4_0": "…pe… ayaṁ imasmiṁ atthe adhippetā bhikkhunīti.",
  "pli-tv-bi-vb-pc74:2.1.5_0": "Ūnadvādasavassā nāma",
  "pli-tv-bi-vb-pc74:2.1.6_0": "appattadvādasavassā.",
  "pli-tv-bi-vb-pc74:2.1.7_0": "Vuṭṭhāpeyyāti",
  "pli-tv-bi-vb-pc74:2.1.8_0": "upasampādeyya.",
  "pli-tv-bi-vb-pc74:2.1.9_0": "“Vuṭṭhāpessāmī”ti gaṇaṁ vā ācariniṁ vā pattaṁ vā cīvaraṁ vā pariyesati, sīmaṁ vā sammannati, āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc74:2.1.10_0": "Ñattiyā dukkaṭaṁ.",
  "pli-tv-bi-vb-pc74:2.1.11_0": "Dvīhi kammavācāhi dukkaṭā.",
  "pli-tv-bi-vb-pc74:2.1.12_0": "Kammavācāpariyosāne upajjhāyāya āpatti pācittiyassa.",
  "pli-tv-bi-vb-pc74:2.1.13_0": "Gaṇassa ca ācariniyā ca āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc74:2.2.1_0": "Anāpatti—",
  "pli-tv-bi-vb-pc74:2.2.2_0": "paripuṇṇadvādasavassā vuṭṭhāpeti,",
  "pli-tv-bi-vb-pc74:2.2.3_0": "ummattikāya,",
  "pli-tv-bi-vb-pc74:2.2.4_0": "ādikammikāyāti.",
  "pli-tv-bi-vb-pc74:2.2.5_0": "Catutthasikkhāpadaṁ niṭṭhitaṁ."
}