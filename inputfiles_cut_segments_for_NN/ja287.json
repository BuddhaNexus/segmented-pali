{
  "ja287:0.1_0": "Jātaka",
  "ja287:0.2_0": "Tikanipāta",
  "ja287:0.3_0": "Abbhantaravagga",
  "ja287:0.4_0": "7. Lābhagarahajātaka",
  "ja287:1.1_0": "“Nānummatto nāpisuṇo,",
  "ja287:1.2_0": "nānaṭo nākutūhalo;",
  "ja287:1.3_0": "Mūḷhesu labhate lābhaṁ,",
  "ja287:1.4_0": "esā te anusāsanī”.",
  "ja287:2.1_0": "“Dhiratthu taṁ yasalābhaṁ,",
  "ja287:2.2_0": "dhanalābhañca brāhmaṇa;",
  "ja287:2.3_0": "Yā vutti vinipātena,",
  "ja287:2.4_0": "adhammacaraṇena vā.",
  "ja287:3.1_0": "Api ce pattamādāya,",
  "ja287:3.2_0": "anagāro paribbaje;",
  "ja287:3.3_0": "Esāva jīvikā seyyo,",
  "ja287:3.4_0": "yā cādhammena esanā”ti.",
  "ja287:4.1_0": "Lābhagarahajātakaṁ sattamaṁ."
}