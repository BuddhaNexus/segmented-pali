{
  "mil3.1.1:0.1_0": "Milindapañha",
  "mil3.1.1:0.2_0": "Mahāvagga",
  "mil3.1.1:0.3_0": "1. Paññattipañha",
  "mil3.1.1:1.1_0": "Atha kho milindo rājā yenāyasmā nāgaseno tenupasaṅkami,",
  "mil3.1.1:1.1_1": "upasaṅkamitvā āyasmatā nāgasenena saddhiṁ sammodi,",
  "mil3.1.1:1.1_2": "sammodanīyaṁ kathaṁ sāraṇīyaṁ vītisāretvā ekamantaṁ nisīdi.",
  "mil3.1.1:1.2_0": "Āyasmāpi kho nāgaseno paṭisammodanīyeneva milindassa rañño cittaṁ ārādhesi.",
  "mil3.1.1:2.1_0": "Atha kho milindo rājā āyasmantaṁ nāgasenaṁ etadavoca—",
  "mil3.1.1:2.2_0": "“kathaṁ bhadanto ñāyati, kinnāmosi, bhante”ti?",
  "mil3.1.1:2.3_0": "“‘Nāgaseno’ti kho ahaṁ, mahārāja, ñāyāmi,",
  "mil3.1.1:2.3_1": "‘nāgaseno’ti kho maṁ, mahārāja,",
  "mil3.1.1:2.3_2": "sabrahmacārī samudācaranti,",
  "mil3.1.1:2.3_3": "api ca mātāpitaro nāmaṁ karonti ‘nāgaseno’ti vā ‘sūraseno’ti vā ‘vīraseno’ti vā ‘sīhaseno’ti vā,",
  "mil3.1.1:2.3_4": "api ca kho, mahārāja,",
  "mil3.1.1:2.3_5": "saṅkhā samaññā paññatti vohāro nāmamattaṁ yadidaṁ nāgasenoti,",
  "mil3.1.1:2.3_6": "na hettha puggalo upalabbhatī”ti.",
  "mil3.1.1:3.1_0": "Atha kho milindo rājā evamāha—",
  "mil3.1.1:3.2_0": "“suṇantu me bhonto pañcasatā yonakā asītisahassā ca bhikkhū, ayaṁ nāgaseno evamāha—",
  "mil3.1.1:3.3_0": "‘na hettha puggalo upalabbhatī’ti, kallaṁ nu kho tadabhinanditun”ti.",
  "mil3.1.1:3.4_0": "Atha kho milindo rājā āyasmantaṁ nāgasenaṁ etadavoca—",
  "mil3.1.1:3.5_0": "“sace,",
  "mil3.1.1:3.5_1": "bhante nāgasena,",
  "mil3.1.1:3.5_2": "puggalo nūpalabbhati,",
  "mil3.1.1:3.5_3": "ko carahi tumhākaṁ cīvarapiṇḍapātasenāsanagilānappaccayabhesajjaparikkhāraṁ deti,",
  "mil3.1.1:3.5_4": "ko taṁ paribhuñjati,",
  "mil3.1.1:3.5_5": "ko sīlaṁ rakkhati,",
  "mil3.1.1:3.5_6": "ko bhāvanamanuyuñjati,",
  "mil3.1.1:3.5_7": "ko maggaphalanibbānāni sacchikaroti,",
  "mil3.1.1:3.5_8": "ko pāṇaṁ hanati,",
  "mil3.1.1:3.5_9": "ko adinnaṁ ādiyati,",
  "mil3.1.1:3.5_10": "ko kāmesumicchācāraṁ carati,",
  "mil3.1.1:3.5_11": "ko musā bhaṇati,",
  "mil3.1.1:3.5_12": "ko majjaṁ pivati,",
  "mil3.1.1:3.5_13": "ko pañcānantariyakammaṁ karoti,",
  "mil3.1.1:3.5_14": "tasmā natthi kusalaṁ,",
  "mil3.1.1:3.5_15": "natthi akusalaṁ,",
  "mil3.1.1:3.5_16": "natthi kusalākusalānaṁ kammānaṁ kattā vā kāretā vā,",
  "mil3.1.1:3.5_17": "natthi sukatadukkaṭānaṁ kammānaṁ phalaṁ vipāko, sace,",
  "mil3.1.1:3.5_18": "bhante nāgasena,",
  "mil3.1.1:3.5_19": "yo tumhe māreti,",
  "mil3.1.1:3.5_20": "natthi tassāpi pāṇātipāto,",
  "mil3.1.1:3.5_21": "tumhākampi,",
  "mil3.1.1:3.5_22": "bhante nāgasena,",
  "mil3.1.1:3.5_23": "natthi ācariyo,",
  "mil3.1.1:3.5_24": "natthi upajjhāyo,",
  "mil3.1.1:3.5_25": "natthi upasampadā.",
  "mil3.1.1:3.6_0": "‘Nāgasenoti maṁ, mahārāja, sabrahmacārī samudācarantī’ti yaṁ vadesi, katamo ettha nāgaseno?",
  "mil3.1.1:3.7_0": "Kiṁ nu kho, bhante, kesā nāgaseno”ti?",
  "mil3.1.1:3.8_0": "“Na hi, mahārājā”ti.",
  "mil3.1.1:3.9_0": "“Lomā nāgaseno”ti?",
  "mil3.1.1:3.10_0": "“Na hi, mahārājā”ti.",
  "mil3.1.1:3.11_0": "“Nakhā …pe…",
  "mil3.1.1:3.12_0": "dantā …",
  "mil3.1.1:3.13_0": "taco …",
  "mil3.1.1:3.14_0": "maṁsaṁ …",
  "mil3.1.1:3.15_0": "nhāru …",
  "mil3.1.1:3.16_0": "aṭṭhi …",
  "mil3.1.1:3.17_0": "aṭṭhimiñjaṁ …",
  "mil3.1.1:3.18_0": "vakkaṁ …",
  "mil3.1.1:3.19_0": "hadayaṁ …",
  "mil3.1.1:3.20_0": "yakanaṁ …",
  "mil3.1.1:3.21_0": "kilomakaṁ …",
  "mil3.1.1:3.22_0": "pihakaṁ …",
  "mil3.1.1:3.23_0": "papphāsaṁ …",
  "mil3.1.1:3.24_0": "antaṁ …",
  "mil3.1.1:3.25_0": "antaguṇaṁ …",
  "mil3.1.1:3.26_0": "udariyaṁ …",
  "mil3.1.1:3.27_0": "karīsaṁ …",
  "mil3.1.1:3.28_0": "pittaṁ …",
  "mil3.1.1:3.29_0": "semhaṁ …",
  "mil3.1.1:3.30_0": "pubbo …",
  "mil3.1.1:3.31_0": "lohitaṁ …",
  "mil3.1.1:3.32_0": "sedo …",
  "mil3.1.1:3.33_0": "medo …",
  "mil3.1.1:3.34_0": "assu …",
  "mil3.1.1:3.35_0": "vasā …",
  "mil3.1.1:3.36_0": "kheḷo …",
  "mil3.1.1:3.37_0": "siṅghāṇikā …",
  "mil3.1.1:3.38_0": "lasikā …",
  "mil3.1.1:3.39_0": "muttaṁ …pe…",
  "mil3.1.1:3.40_0": "matthake matthaluṅgaṁ nāgaseno”ti?",
  "mil3.1.1:3.41_0": "“Na hi, mahārājā”ti.",
  "mil3.1.1:3.42_0": "“Kiṁ nu kho, bhante, rūpaṁ nāgaseno”ti?",
  "mil3.1.1:3.43_0": "“Na hi, mahārājā”ti.",
  "mil3.1.1:3.44_0": "“Vedanā nāgaseno”ti?",
  "mil3.1.1:3.45_0": "“Na hi, mahārājā”ti.",
  "mil3.1.1:3.46_0": "“Saññā nāgaseno”ti?",
  "mil3.1.1:3.47_0": "“Na hi, mahārājā”ti.",
  "mil3.1.1:3.48_0": "“Saṅkhārā nāgaseno”ti?",
  "mil3.1.1:3.49_0": "“Na hi, mahārājā”ti.",
  "mil3.1.1:3.50_0": "“Viññāṇaṁ nāgaseno”ti?",
  "mil3.1.1:3.51_0": "“Na hi, mahārājā”ti.",
  "mil3.1.1:3.52_0": "“Kiṁ pana, bhante, rūpavedanāsaññāsaṅkhāraviññāṇaṁ nāgaseno”ti?",
  "mil3.1.1:3.53_0": "“Na hi, mahārājā”ti.",
  "mil3.1.1:3.54_0": "“Kiṁ pana, bhante, aññatra rūpavedanāsaññāsaṅkhāraviññāṇaṁ nāgaseno”ti?",
  "mil3.1.1:3.55_0": "“Na hi, mahārājā”ti.",
  "mil3.1.1:3.56_0": "“Tamahaṁ, bhante, pucchanto pucchanto na passāmi nāgasenaṁ.",
  "mil3.1.1:3.57_0": "Nāgasenasaddoyeva nu kho, bhante, nāgaseno”ti?",
  "mil3.1.1:3.58_0": "“Na hi, mahārājā”ti.",
  "mil3.1.1:3.59_0": "“Ko panettha nāgaseno, alikaṁ tvaṁ, bhante, bhāsasi musāvādaṁ, natthi nāgaseno”ti.",
  "mil3.1.1:4.1_0": "Atha kho āyasmā nāgaseno milindaṁ rājānaṁ etadavoca—",
  "mil3.1.1:4.2_0": "“tvaṁ khosi, mahārāja,",
  "mil3.1.1:4.2_1": "khattiyasukhumālo accantasukhumālo, tassa te, mahārāja,",
  "mil3.1.1:4.2_2": "majjhanhikasamayaṁ tattāya bhūmiyā uṇhāya vālikāya kharāya sakkharakathalikāya madditvā pādenāgacchantassa pādā rujjanti,",
  "mil3.1.1:4.2_3": "kāyo kilamati,",
  "mil3.1.1:4.2_4": "cittaṁ upahaññati,",
  "mil3.1.1:4.2_5": "dukkhasahagataṁ kāyaviññāṇaṁ uppajjati,",
  "mil3.1.1:4.2_6": "kiṁ nu kho tvaṁ pādenāgatosi,",
  "mil3.1.1:4.2_7": "udāhu vāhanenā”ti?",
  "mil3.1.1:4.3_0": "“Nāhaṁ, bhante, pādenāgacchāmi, rathenāhaṁ āgatosmī”ti.",
  "mil3.1.1:4.4_0": "“Sace tvaṁ, mahārāja, rathenāgatosi, rathaṁ me ārocehi, kiṁ nu kho, mahārāja, īsā ratho”ti?",
  "mil3.1.1:4.5_0": "“Na hi, bhante”ti.",
  "mil3.1.1:4.6_0": "“Akkho ratho”ti?",
  "mil3.1.1:4.7_0": "“Na hi, bhante”ti.",
  "mil3.1.1:4.8_0": "“Cakkāni ratho”ti?",
  "mil3.1.1:4.9_0": "“Na hi, bhante”ti.",
  "mil3.1.1:4.10_0": "“Rathapañjaraṁ ratho”ti?",
  "mil3.1.1:4.11_0": "“Na hi, bhante”ti.",
  "mil3.1.1:4.12_0": "“Rathadaṇḍako ratho”ti?",
  "mil3.1.1:4.13_0": "“Na hi, bhante”ti.",
  "mil3.1.1:4.14_0": "“Yugaṁ ratho”ti?",
  "mil3.1.1:4.15_0": "“Na hi, bhante”ti.",
  "mil3.1.1:4.16_0": "“Rasmiyo ratho”ti?",
  "mil3.1.1:4.17_0": "“Na hi, bhante”ti.",
  "mil3.1.1:4.18_0": "“Patodalaṭṭhi ratho”ti?",
  "mil3.1.1:4.19_0": "“Na hi, bhante”ti.",
  "mil3.1.1:4.20_0": "“Kiṁ nu kho, mahārāja, īsāakkhacakkarathapañjararathadaṇḍayugarasmipatodā ratho”ti?",
  "mil3.1.1:4.21_0": "“Na hi, bhante”ti.",
  "mil3.1.1:4.22_0": "“Kiṁ pana, mahārāja, aññatra īsāakkhacakkarathapañjararathadaṇḍayugarasmipatodā ratho”ti?",
  "mil3.1.1:4.23_0": "“Na hi, bhante”ti.",
  "mil3.1.1:4.24_0": "“Tamahaṁ, mahārāja, pucchanto pucchanto na passāmi rathaṁ.",
  "mil3.1.1:4.25_0": "Rathasaddoyeva nu kho, mahārāja, ratho”ti?",
  "mil3.1.1:4.26_0": "“Na hi, bhante”ti.",
  "mil3.1.1:4.27_0": "“Ko panettha ratho,",
  "mil3.1.1:4.27_1": "alikaṁ tvaṁ, mahārāja,",
  "mil3.1.1:4.27_2": "bhāsasi musāvādaṁ,",
  "mil3.1.1:4.27_3": "natthi ratho, tvaṁsi, mahārāja,",
  "mil3.1.1:4.27_4": "sakalajambudīpe aggarājā,",
  "mil3.1.1:4.27_5": "kassa pana tvaṁ bhāyitvā musāvādaṁ bhāsasi,",
  "mil3.1.1:4.27_6": "suṇantu me bhonto pañcasatā yonakā asītisahassā ca bhikkhū,",
  "mil3.1.1:4.27_7": "ayaṁ milindo rājā evamāha—",
  "mil3.1.1:4.28_0": "‘rathenāhaṁ āgatosmī’ti,",
  "mil3.1.1:4.28_1": "sace tvaṁ, mahārāja,",
  "mil3.1.1:4.28_2": "rathenāgatosi,",
  "mil3.1.1:4.28_3": "‘rathaṁ me ārocehī’ti vutto samāno rathaṁ na sampādeti,",
  "mil3.1.1:4.28_4": "kallaṁ nu kho tadabhinanditun”ti.",
  "mil3.1.1:5.1_0": "Evaṁ vutte, pañcasatā yonakā āyasmato nāgasenassa sādhukāraṁ datvā milindaṁ rājānaṁ etadavocuṁ—",
  "mil3.1.1:5.2_0": "“idāni kho tvaṁ, mahārāja, sakkonto bhāsassū”ti.",
  "mil3.1.1:6.1_0": "Atha kho milindo rājā āyasmantaṁ nāgasenaṁ etadavoca—",
  "mil3.1.1:6.2_0": "“nāhaṁ,",
  "mil3.1.1:6.2_1": "bhante nāgasena,",
  "mil3.1.1:6.2_2": "musā bhaṇāmi,",
  "mil3.1.1:6.2_3": "īsañca paṭicca akkhañca paṭicca cakkāni ca paṭicca rathapañjarañca paṭicca rathadaṇḍakañca paṭicca ‘ratho’ti",
  "mil3.1.1:6.2_4": "saṅkhā samaññā paññatti vohāro nāmamattaṁ pavattatī”ti.",
  "mil3.1.1:7.1_0": "“Sādhu kho tvaṁ, mahārāja, rathaṁ jānāsi;",
  "mil3.1.1:7.2_0": "evameva kho, mahārāja,",
  "mil3.1.1:7.2_1": "mayhampi kese ca paṭicca lome ca paṭicca …pe…",
  "mil3.1.1:7.2_2": "matthake matthaluṅgañca paṭicca rūpañca paṭicca vedanañca paṭicca saññañca paṭicca saṅkhāre ca paṭicca",
  "mil3.1.1:7.2_3": "viññāṇañca paṭicca ‘nāgaseno’ti saṅkhā samaññā paññatti vohāro nāmamattaṁ pavattati,",
  "mil3.1.1:7.2_4": "paramatthato panettha puggalo nūpalabbhati.",
  "mil3.1.1:7.3_0": "Bhāsitampetaṁ, mahārāja, vajirāya bhikkhuniyā bhagavato sammukhā—",
  "mil3.1.1:8.1_0": "‘Yathā hi aṅgasambhārā,",
  "mil3.1.1:8.2_0": "hoti saddo ratho iti;",
  "mil3.1.1:8.3_0": "Evaṁ khandhesu santesu,",
  "mil3.1.1:8.4_0": "hoti “satto”ti sammutī’”ti.",
  "mil3.1.1:9.1_0": "“Acchariyaṁ,",
  "mil3.1.1:9.1_1": "bhante nāgasena, abbhutaṁ,",
  "mil3.1.1:9.1_2": "bhante nāgasena,",
  "mil3.1.1:9.1_3": "aticitrāni pañhapaṭibhānāni visajjitāni,",
  "mil3.1.1:9.1_4": "yadi buddho tiṭṭheyya sādhukāraṁ dadeyya,",
  "mil3.1.1:9.1_5": "sādhu sādhu, nāgasena,",
  "mil3.1.1:9.1_6": "aticitrāni pañhapaṭibhānāni visajjitānī”ti.",
  "mil3.1.1:10.1_0": "Paññattipañho paṭhamo."
}