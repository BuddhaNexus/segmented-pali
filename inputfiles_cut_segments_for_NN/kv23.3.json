{
  "kv23.3:0.1_0": "Kathāvatthu",
  "kv23.3:0.2_0": "Mahāpaṇṇāsaka",
  "kv23.3:0.3_0": "Tevīsatimavagga",
  "kv23.3:0.4_0": "Issariyakāmakārikādikathā",
  "kv23.3:1.1_0": "Bodhisatto issariyakāmakārikāhetu vinipātaṁ gacchatīti?",
  "kv23.3:1.2_0": "Āmantā.",
  "kv23.3:1.3_0": "Bodhisatto issariyakāmakārikāhetu nirayaṁ gacchati,",
  "kv23.3:1.4_0": "sañjīvaṁ gacchati,",
  "kv23.3:1.5_0": "kālasuttaṁ gacchati,",
  "kv23.3:1.6_0": "tāpanaṁ gacchati,",
  "kv23.3:1.7_0": "mahātāpanaṁ gacchati,",
  "kv23.3:1.8_0": "saṅghātakaṁ gacchati,",
  "kv23.3:1.9_0": "roruvaṁ gacchati …pe…",
  "kv23.3:1.10_0": "avīciṁ gacchatīti?",
  "kv23.3:1.11_0": "Na hevaṁ vattabbe …pe….",
  "kv23.3:2.1_0": "Bodhisatto issariyakāmakārikāhetu vinipātaṁ gacchatīti?",
  "kv23.3:2.2_0": "Āmantā.",
  "kv23.3:2.3_0": "“Bodhisatto issariyakāmakārikāhetu vinipātaṁ gacchatī”ti—",
  "kv23.3:2.4_0": "attheva suttantoti?",
  "kv23.3:2.5_0": "Natthi.",
  "kv23.3:2.6_0": "Hañci “bodhisatto issariyakāmakārikāhetu vinipātaṁ gacchatī”ti—",
  "kv23.3:2.7_0": "nattheva suttanto, no ca vata re vattabbe—",
  "kv23.3:2.8_0": "“bodhisatto issariyakāmakārikāhetu vinipātaṁ gacchatī”ti.",
  "kv23.3:3.1_0": "Bodhisatto issariyakāmakārikāhetu gabbhaseyyaṁ okkamatīti?",
  "kv23.3:3.2_0": "Āmantā.",
  "kv23.3:3.3_0": "Bodhisatto issariyakāmakārikāhetu nirayaṁ upapajjeyya, tiracchānayoniṁ upapajjeyyāti?",
  "kv23.3:3.4_0": "Na hevaṁ vattabbe …pe….",
  "kv23.3:4.1_0": "Bodhisatto issariyakāmakārikāhetu gabbhaseyyaṁ okkamatīti?",
  "kv23.3:4.2_0": "Āmantā.",
  "kv23.3:4.3_0": "Bodhisatto iddhimāti?",
  "kv23.3:4.4_0": "Na hevaṁ vattabbe …pe…",
  "kv23.3:4.5_0": "bodhisatto iddhimāti?",
  "kv23.3:4.6_0": "Āmantā.",
  "kv23.3:4.7_0": "Bodhisattena chandiddhipādo bhāvito …pe…",
  "kv23.3:4.8_0": "vīriyiddhipādo …pe…",
  "kv23.3:4.9_0": "cittiddhipādo …pe…",
  "kv23.3:4.10_0": "vīmaṁsiddhipādo bhāvitoti?",
  "kv23.3:4.11_0": "Na hevaṁ vattabbe …pe….",
  "kv23.3:5.1_0": "Bodhisatto issariyakāmakārikāhetu gabbhaseyyaṁ okkamatīti?",
  "kv23.3:5.2_0": "Āmantā.",
  "kv23.3:5.3_0": "“Bodhisatto issariyakāmakārikāhetu gabbhaseyyaṁ okkamatī”ti—",
  "kv23.3:5.4_0": "attheva suttantoti?",
  "kv23.3:5.5_0": "Natthi.",
  "kv23.3:5.6_0": "Hañci “bodhisatto issariyakāmakārikāhetu gabbhaseyyaṁ okkamatī”ti—",
  "kv23.3:5.7_0": "nattheva suttanto, no ca vata re vattabbe—",
  "kv23.3:5.8_0": "“bodhisatto issariyakāmakārikāhetu gabbhaseyyaṁ okkamatī”ti.",
  "kv23.3:6.1_0": "Bodhisatto issariyakāmakārikāhetu dukkarakārikaṁ akāsīti?",
  "kv23.3:6.2_0": "Āmantā.",
  "kv23.3:6.3_0": "Bodhisatto issariyakāmakārikāhetu “sassato loko”ti paccāgacchi,",
  "kv23.3:6.4_0": "“asassato loko”ti …pe…",
  "kv23.3:6.5_0": "“antavā loko”ti …pe…",
  "kv23.3:6.6_0": "“anantavā loko”ti …",
  "kv23.3:6.7_0": "“taṁ jīvaṁ taṁ sarīran”ti …",
  "kv23.3:6.8_0": "“aññaṁ jīvaṁ aññaṁ sarīran”ti …",
  "kv23.3:6.9_0": "“hoti tathāgato paraṁ maraṇā”ti …",
  "kv23.3:6.10_0": "“na hoti tathāgato paraṁ maraṇā”ti …",
  "kv23.3:6.11_0": "“hoti ca na ca hoti tathāgato paraṁ maraṇā”ti …pe…",
  "kv23.3:6.12_0": "“neva hoti na na hoti tathāgato paraṁ maraṇā”ti paccāgacchīti?",
  "kv23.3:6.13_0": "Na hevaṁ vattabbe …pe….",
  "kv23.3:7.1_0": "Bodhisatto issariyakāmakārikāhetu dukkarakārikaṁ akāsīti?",
  "kv23.3:7.2_0": "Āmantā.",
  "kv23.3:7.3_0": "“Bodhisatto issariyakāmakārikāhetu dukkarakārikaṁ akāsī”ti—",
  "kv23.3:7.4_0": "attheva suttantoti?",
  "kv23.3:7.5_0": "Natthi.",
  "kv23.3:7.6_0": "Hañci “bodhisatto issariyakāmakārikāhetu dukkarakārikaṁ akāsī”ti—",
  "kv23.3:7.7_0": "nattheva suttanto, no ca vata re vattabbe—",
  "kv23.3:7.8_0": "“bodhisatto issariyakāmakārikāhetu dukkarakārikaṁ akāsī”ti.",
  "kv23.3:8.1_0": "Bodhisatto issariyakāmakārikāhetu aparantapaṁ akāsi, aññaṁ satthāraṁ uddisīti?",
  "kv23.3:8.2_0": "Āmantā.",
  "kv23.3:8.3_0": "Bodhisatto issariyakāmakārikāhetu “sassato loko”ti paccāgacchi …pe…",
  "kv23.3:8.4_0": "“neva hoti na na hoti tathāgato paraṁ maraṇā”ti paccāgacchīti?",
  "kv23.3:8.5_0": "Na hevaṁ vattabbe …pe….",
  "kv23.3:9.1_0": "Bodhisatto issariyakāmakārikāhetu aññaṁ satthāraṁ uddisīti?",
  "kv23.3:9.2_0": "Āmantā.",
  "kv23.3:9.3_0": "“Bodhisatto issariyakāmakārikāhetu aññaṁ satthāraṁ uddisī”ti—",
  "kv23.3:9.4_0": "attheva suttantoti?",
  "kv23.3:9.5_0": "Natthi.",
  "kv23.3:9.6_0": "Hañci “bodhisatto issariyakāmakārikāhetu aññaṁ satthāraṁ uddisī”ti—",
  "kv23.3:9.7_0": "nattheva suttanto, no ca vata re vattabbe—",
  "kv23.3:9.8_0": "“bodhisatto issariyakāmakārikāhetu aññaṁ satthāraṁ uddisī”ti.",
  "kv23.3:9.9_0": "Issariyakāmakārikākathā niṭṭhitā."
}