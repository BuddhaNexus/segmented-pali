{
  "kv13.10:0.1_0": "Kathāvatthu",
  "kv13.10:0.2_0": "Mahāpaṇṇāsaka",
  "kv13.10:0.3_0": "Terasamavagga",
  "kv13.10:0.4_0": "Dhammataṇhānadukkhasamudayotikathā",
  "kv13.10:1.1_0": "Dhammataṇhā na dukkhasamudayoti?",
  "kv13.10:1.2_0": "Āmantā.",
  "kv13.10:1.3_0": "Rūpataṇhā na dukkhasamudayoti?",
  "kv13.10:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv13.10:1.5_0": "dhammataṇhā na dukkhasamudayoti?",
  "kv13.10:1.6_0": "Āmantā.",
  "kv13.10:1.7_0": "Saddataṇhā …pe…",
  "kv13.10:1.8_0": "gandhataṇhā …pe…",
  "kv13.10:1.9_0": "rasataṇhā …pe…",
  "kv13.10:1.10_0": "phoṭṭhabbataṇhā na dukkhasamudayoti?",
  "kv13.10:1.11_0": "Na hevaṁ vattabbe …pe….",
  "kv13.10:2.1_0": "Rūpataṇhā dukkhasamudayoti?",
  "kv13.10:2.2_0": "Āmantā.",
  "kv13.10:2.3_0": "Dhammataṇhā dukkhasamudayoti?",
  "kv13.10:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv13.10:2.5_0": "saddataṇhā …pe…",
  "kv13.10:2.6_0": "gandhataṇhā …pe…",
  "kv13.10:2.7_0": "rasataṇhā …pe…",
  "kv13.10:2.8_0": "phoṭṭhabbataṇhā dukkhasamudayoti?",
  "kv13.10:2.9_0": "Āmantā.",
  "kv13.10:2.10_0": "Dhammataṇhā dukkhasamudayoti?",
  "kv13.10:2.11_0": "Na hevaṁ vattabbe …pe….",
  "kv13.10:3.1_0": "Dhammataṇhā na dukkhasamudayoti?",
  "kv13.10:3.2_0": "Āmantā.",
  "kv13.10:3.3_0": "Nanu taṇhā dukkhasamudayo vutto bhagavatāti?",
  "kv13.10:3.4_0": "Āmantā.",
  "kv13.10:3.5_0": "Hañci taṇhā dukkhasamudayo vutto bhagavatā, no ca vata re vattabbe—",
  "kv13.10:3.6_0": "“dhammataṇhā na dukkhasamudayo”ti.",
  "kv13.10:3.7_0": "Dhammataṇhā na dukkhasamudayoti?",
  "kv13.10:3.8_0": "Āmantā.",
  "kv13.10:3.9_0": "Nanu lobho dukkhasamudayo vutto bhagavatā, dhammataṇhā lobhoti?",
  "kv13.10:3.10_0": "Āmantā.",
  "kv13.10:3.11_0": "Hañci lobho dukkhasamudayo vutto bhagavatā, dhammataṇhā lobho, no ca vata re vattabbe—",
  "kv13.10:3.12_0": "“dhammataṇhā na dukkhasamudayo”ti.",
  "kv13.10:4.1_0": "Dhammataṇhā lobho, na dukkhasamudayoti?",
  "kv13.10:4.2_0": "Āmantā.",
  "kv13.10:4.3_0": "Rūpataṇhā lobho, na dukkhasamudayoti?",
  "kv13.10:4.4_0": "Na hevaṁ vattabbe …pe…",
  "kv13.10:4.5_0": "dhammataṇhā lobho, na dukkhasamudayoti?",
  "kv13.10:4.6_0": "Āmantā.",
  "kv13.10:4.7_0": "Saddataṇhā …pe…",
  "kv13.10:4.8_0": "gandhataṇhā …pe…",
  "kv13.10:4.9_0": "rasataṇhā …pe…",
  "kv13.10:4.10_0": "phoṭṭhabbataṇhā lobho, na dukkhasamudayoti?",
  "kv13.10:4.11_0": "Na hevaṁ vattabbe …pe….",
  "kv13.10:5.1_0": "Rūpataṇhā lobho dukkhasamudayoti?",
  "kv13.10:5.2_0": "Āmantā.",
  "kv13.10:5.3_0": "Dhammataṇhā lobho dukkhasamudayoti?",
  "kv13.10:5.4_0": "Na hevaṁ vattabbe …pe…",
  "kv13.10:5.5_0": "saddataṇhā …pe…",
  "kv13.10:5.6_0": "phoṭṭhabbataṇhā lobho dukkhasamudayoti?",
  "kv13.10:5.7_0": "Āmantā.",
  "kv13.10:5.8_0": "Dhammataṇhā lobho dukkhasamudayoti?",
  "kv13.10:5.9_0": "Na hevaṁ vattabbe …pe….",
  "kv13.10:6.1_0": "Dhammataṇhā na dukkhasamudayoti?",
  "kv13.10:6.2_0": "Āmantā.",
  "kv13.10:6.3_0": "Nanu vuttaṁ bhagavatā—",
  "kv13.10:6.4_0": "“yāyaṁ taṇhā ponobbhavikā nandīrāgasahagatā tatratatrābhinandinī, seyyathidaṁ—",
  "kv13.10:6.5_0": "kāmataṇhā, bhavataṇhā, vibhavataṇhā”ti.",
  "kv13.10:6.6_0": "Attheva suttantoti?",
  "kv13.10:6.7_0": "Āmantā.",
  "kv13.10:6.8_0": "Tena hi na vattabbaṁ—",
  "kv13.10:6.9_0": "“dhammataṇhā na dukkhasamudayo”ti.",
  "kv13.10:7.1_0": "Na vattabbaṁ—",
  "kv13.10:7.2_0": "“dhammataṇhā na dukkhasamudayo”ti?",
  "kv13.10:7.3_0": "Āmantā.",
  "kv13.10:7.4_0": "Nanu sā dhammataṇhāti?",
  "kv13.10:7.5_0": "Āmantā.",
  "kv13.10:7.6_0": "Hañci sā dhammataṇhā, tena vata re vattabbe—",
  "kv13.10:7.7_0": "“dhammataṇhā na dukkhasamudayo”ti.",
  "kv13.10:7.8_0": "Dhammataṇhā na dukkhasamudayotikathā niṭṭhitā.",
  "kv13.10:7.9_0": "Terasamo vaggo.",
  "kv13.10:7.10_0": "Tassuddānaṁ",
  "kv13.10:8.1_0": "Kappaṭṭho kappaṁ tiṭṭheyya,",
  "kv13.10:8.1_1": "kappaṭṭho kusalaṁ cittaṁ na paṭilabheyya,",
  "kv13.10:8.1_2": "anantarāpayutto puggalo sammattaniyāmaṁ okkameyya,",
  "kv13.10:8.1_3": "niyato niyāmaṁ okkamati,",
  "kv13.10:8.1_4": "nivuto nīvaraṇaṁ jahati,",
  "kv13.10:8.1_5": "sammukhībhūto saṁyojanaṁ jahati,",
  "kv13.10:8.1_6": "jhānanikanti,",
  "kv13.10:8.1_7": "asātarāgo,",
  "kv13.10:8.1_8": "dhammataṇhā abyākatā,",
  "kv13.10:8.1_9": "dhammataṇhā na dukkhasamudayoti."
}