{
  "ja364:0.1_0": "Jātaka",
  "ja364:0.2_0": "Pañcakanipāta",
  "ja364:0.3_0": "Vaṇṇārohavagga",
  "ja364:0.4_0": "4. Khajjopanakajātaka",
  "ja364:1.1_0": "“Ko nu santamhi pajjote,",
  "ja364:1.2_0": "aggipariyesanaṁ caraṁ;",
  "ja364:1.3_0": "Addakkhi ratti khajjotaṁ,",
  "ja364:1.4_0": "jātavedaṁ amaññatha.",
  "ja364:2.1_0": "Svassa gomayacuṇṇāni,",
  "ja364:2.2_0": "abhimatthaṁ tiṇāni ca;",
  "ja364:2.3_0": "Viparītāya saññāya,",
  "ja364:2.4_0": "nāsakkhi pajjaletave.",
  "ja364:3.1_0": "Evampi anupāyena,",
  "ja364:3.2_0": "atthaṁ na labhate migo;",
  "ja364:3.3_0": "Visāṇato gavaṁ dohaṁ,",
  "ja364:3.4_0": "yattha khīraṁ na vindati.",
  "ja364:4.1_0": "Vividhehi upāyehi,",
  "ja364:4.2_0": "atthaṁ papponti māṇavā;",
  "ja364:4.3_0": "Niggahena amittānaṁ,",
  "ja364:4.4_0": "mittānaṁ paggahena ca.",
  "ja364:5.1_0": "Senāmokkhapalābhena,",
  "ja364:5.2_0": "vallabhānaṁ nayena ca;",
  "ja364:5.3_0": "Jagatiṁ jagatipālā,",
  "ja364:5.4_0": "āvasanti vasundharan”ti.",
  "ja364:6.1_0": "Khajjopanakajātakaṁ catutthaṁ."
}