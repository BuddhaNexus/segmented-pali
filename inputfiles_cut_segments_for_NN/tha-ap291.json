{
  "tha-ap291:0.1_0": "Therāpadāna",
  "tha-ap291:0.2_0": "Paṇṇadāyakavagga",
  "tha-ap291:0.3_0": "9 Buddhupaṭṭhāyikattheraapadāna",
  "tha-ap291:1.1_0": "“Veṭambhinīti me nāmaṁ,",
  "tha-ap291:1.2_0": "pitusantaṁ mamaṁ tadā;",
  "tha-ap291:1.3_0": "Mama hatthaṁ gahetvāna,",
  "tha-ap291:1.4_0": "upānayi mahāmuniṁ.",
  "tha-ap291:2.1_0": "Imemaṁ uddisissanti,",
  "tha-ap291:2.2_0": "buddhā lokagganāyakā;",
  "tha-ap291:2.3_0": "Tehaṁ upaṭṭhiṁ sakkaccaṁ,",
  "tha-ap291:2.4_0": "pasanno sehi pāṇibhi.",
  "tha-ap291:3.1_0": "Ekattiṁse ito kappe,",
  "tha-ap291:3.2_0": "buddhe upaṭṭhahiṁ tadā;",
  "tha-ap291:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap291:3.4_0": "upaṭṭhānassidaṁ phalaṁ.",
  "tha-ap291:4.1_0": "Tevīsamhi ito kappe,",
  "tha-ap291:4.2_0": "caturo āsu khattiyā;",
  "tha-ap291:4.3_0": "Samaṇupaṭṭhākā nāma,",
  "tha-ap291:4.4_0": "cakkavattī mahabbalā.",
  "tha-ap291:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap291:5.2_0": "…pe…",
  "tha-ap291:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap291:6.1_0": "Itthaṁ sudaṁ āyasmā buddhupaṭṭhāyiko thero imā gāthāyo abhāsitthāti.",
  "tha-ap291:7.1_0": "Buddhupaṭṭhāyikattherassāpadānaṁ navamaṁ."
}