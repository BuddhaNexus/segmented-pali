{
  "patthana17.17:0.1_0": "Paṭṭhānapakaraṇa",
  "patthana17.17:0.2_0": "Dhammānulomapaccanīya (3), Tikatikapaṭṭhānapāḷi (5)",
  "patthana17.17:0.3_0": "17.17. Kusalattika, Atītattika",
  "patthana17.17:1.1_0": "Kusalo atīto dhammo nakusalassa naatītassa dhammassa ārammaṇapaccayena paccayo. (Saṅkhittaṁ.)",
  "patthana17.17:2.1_0": "Ārammaṇe aṭṭhārasa,",
  "patthana17.17:2.1_1": "adhipatiyā ekādasa,",
  "patthana17.17:2.1_2": "anantare samanantare soḷasa,",
  "patthana17.17:2.1_3": "upanissaye aṭṭhārasa,",
  "patthana17.17:2.1_4": "natthiyā vigate soḷasa.",
  "patthana17.17:2.1_5": "(Anāgataṁ atītasadisaṁ.)"
}