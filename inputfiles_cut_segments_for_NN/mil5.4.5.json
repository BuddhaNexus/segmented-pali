{
  "mil5.4.5:0.1_0": "Milindapañha",
  "mil5.4.5:0.2_0": "Meṇḍakapañha",
  "mil5.4.5:0.3_0": "Sabbaññutañāṇavagga",
  "mil5.4.5:0.4_0": "5. Attanipātanapañha",
  "mil5.4.5:1.1_0": "“Bhante nāgasena, bhāsitampetaṁ bhagavatā—",
  "mil5.4.5:1.2_0": "‘na, bhikkhave, attānaṁ pātetabbaṁ, yo pāteyya, yathādhammo kāretabbo’ti.",
  "mil5.4.5:1.3_0": "Puna ca tumhe bhaṇatha—",
  "mil5.4.5:1.4_0": "‘yattha katthaci bhagavā sāvakānaṁ dhammaṁ desayamāno anekapariyāyena jātiyā jarāya byādhino maraṇassa samucchedāya dhammaṁ deseti,",
  "mil5.4.5:1.4_1": "yo hi koci jātijarābyādhimaraṇaṁ samatikkamati,",
  "mil5.4.5:1.4_2": "taṁ paramāya pasaṁsāya pasaṁsatī’ti.",
  "mil5.4.5:1.5_0": "Yadi, bhante nāgasena, bhagavatā bhaṇitaṁ—",
  "mil5.4.5:1.6_0": "‘na,",
  "mil5.4.5:1.6_1": "bhikkhave,",
  "mil5.4.5:1.6_2": "attānaṁ pātetabbaṁ,",
  "mil5.4.5:1.6_3": "yo pāteyya,",
  "mil5.4.5:1.6_4": "yathādhammo kāretabbo’ti,",
  "mil5.4.5:1.6_5": "tena hi ‘jātiyā jarāya byādhino maraṇassa samucchedāya dhammaṁ desetī’ti yaṁ vacanaṁ,",
  "mil5.4.5:1.6_6": "taṁ micchā.",
  "mil5.4.5:1.7_0": "Yadi jātiyā jarāya byādhino maraṇassa samucchedāya dhammaṁ deseti,",
  "mil5.4.5:1.7_1": "tena hi ‘na,",
  "mil5.4.5:1.7_2": "bhikkhave,",
  "mil5.4.5:1.7_3": "attānaṁ pātetabbaṁ,",
  "mil5.4.5:1.7_4": "yo pāteyya,",
  "mil5.4.5:1.7_5": "yathādhammo kāretabbo’ti tampi vacanaṁ micchā.",
  "mil5.4.5:1.8_0": "Ayampi ubhato koṭiko pañho tavānuppatto, so tayā nibbāhitabbo”ti.",
  "mil5.4.5:2.1_0": "“Bhāsitampetaṁ, mahārāja, bhagavatā—",
  "mil5.4.5:2.2_0": "‘na, bhikkhave, attānaṁ pātetabbaṁ, yo pāteyya, yathādhammo kāretabbo’ti.",
  "mil5.4.5:2.3_0": "Yattha katthaci bhagavatā sāvakānaṁ dhammaṁ desayamānena ca anekapariyāyena jātiyā jarāya byādhino maraṇassa samucchedāya dhammo desito,",
  "mil5.4.5:2.3_1": "tattha pana kāraṇaṁ atthi,",
  "mil5.4.5:2.3_2": "yena bhagavā kāraṇena paṭikkhipi samādapesi cā”ti.",
  "mil5.4.5:3.1_0": "“Kiṁ panettha, bhante nāgasena, kāraṇaṁ, yena bhagavā kāraṇena paṭikkhipi samādapesi cā”ti?",
  "mil5.4.5:3.2_0": "“Sīlavā, mahārāja,",
  "mil5.4.5:3.2_1": "sīlasampanno agadasamo sattānaṁ kilesavisavināsane,",
  "mil5.4.5:3.2_2": "osadhasamo sattānaṁ kilesabyādhivūpasame,",
  "mil5.4.5:3.2_3": "udakasamo sattānaṁ kilesarajojallāpaharaṇe,",
  "mil5.4.5:3.2_4": "maṇiratanasamo sattānaṁ sabbasampattidāne,",
  "mil5.4.5:3.2_5": "nāvāsamo sattānaṁ caturoghapāragamane,",
  "mil5.4.5:3.2_6": "satthavāhasamo sattānaṁ jātikantāratāraṇe,",
  "mil5.4.5:3.2_7": "vātasamo sattānaṁ tividhaggisantāpanibbāpane,",
  "mil5.4.5:3.2_8": "mahāmeghasamo sattānaṁ mānasaparipūraṇe,",
  "mil5.4.5:3.2_9": "ācariyasamo sattānaṁ kusalasikkhāpane,",
  "mil5.4.5:3.2_10": "sudesakasamo sattānaṁ khemapathamācikkhaṇe.",
  "mil5.4.5:3.3_0": "Evarūpo, mahārāja,",
  "mil5.4.5:3.3_1": "bahuguṇo anekaguṇo appamāṇaguṇo guṇarāsi guṇapuñjo sattānaṁ vaḍḍhikaro sīlavā ‘mā vinassī’ti sattānaṁ anukampāya bhagavā sikkhāpadaṁ paññapesi ‘na,",
  "mil5.4.5:3.3_2": "bhikkhave,",
  "mil5.4.5:3.3_3": "attānaṁ pātetabbaṁ,",
  "mil5.4.5:3.3_4": "yo pāteyya,",
  "mil5.4.5:3.3_5": "yathādhammo kāretabbo’ti.",
  "mil5.4.5:3.4_0": "Idamettha, mahārāja, kāraṇaṁ, yena kāraṇena bhagavā paṭikkhipi.",
  "mil5.4.5:3.5_0": "Bhāsitampetaṁ, mahārāja,",
  "mil5.4.5:3.5_1": "therena kumārakassapena vicitrakathikena pāyāsirājaññassa paralokaṁ dīpayamānena ‘yathā yathā kho, rājañña,",
  "mil5.4.5:3.5_2": "samaṇabrāhmaṇā sīlavanto kalyāṇadhammā ciraṁ dīghamaddhānaṁ tiṭṭhanti tathā tathā bahuṁ puññaṁ pasavanti,",
  "mil5.4.5:3.5_3": "bahujanahitāya ca paṭipajjanti bahujanasukhāya lokānukampāya atthāya hitāya sukhāya devamanussānan’ti.",
  "mil5.4.5:4.1_0": "Kena pana kāraṇena bhagavā samādapesi?",
  "mil5.4.5:4.2_0": "Jātipi, mahārāja, dukkhā,",
  "mil5.4.5:4.2_1": "jarāpi dukkhā,",
  "mil5.4.5:4.2_2": "byādhipi dukkho,",
  "mil5.4.5:4.2_3": "maraṇampi dukkhaṁ,",
  "mil5.4.5:4.2_4": "sokopi dukkho,",
  "mil5.4.5:4.2_5": "paridevopi dukkho,",
  "mil5.4.5:4.2_6": "dukkhampi dukkhaṁ,",
  "mil5.4.5:4.2_7": "domanassampi dukkhaṁ,",
  "mil5.4.5:4.2_8": "upāyāsopi dukkho,",
  "mil5.4.5:4.2_9": "appiyehi sampayogopi dukkho,",
  "mil5.4.5:4.2_10": "piyehi vippayogopi dukkho,",
  "mil5.4.5:4.2_11": "mātumaraṇampi dukkhaṁ,",
  "mil5.4.5:4.2_12": "pitumaraṇampi dukkhaṁ,",
  "mil5.4.5:4.2_13": "bhātumaraṇampi dukkhaṁ,",
  "mil5.4.5:4.2_14": "bhaginimaraṇampi dukkhaṁ,",
  "mil5.4.5:4.2_15": "puttamaraṇampi dukkhaṁ,",
  "mil5.4.5:4.2_16": "dāramaraṇampi dukkhaṁ,",
  "mil5.4.5:4.2_17": "dāsamaraṇampi dukkhaṁ,",
  "mil5.4.5:4.2_18": "ñātimaraṇampi dukkhaṁ,",
  "mil5.4.5:4.2_19": "ñātibyasanampi dukkhaṁ,",
  "mil5.4.5:4.2_20": "rogabyasanampi dukkhaṁ,",
  "mil5.4.5:4.2_21": "bhogabyasanampi dukkhaṁ,",
  "mil5.4.5:4.2_22": "sīlabyasanampi dukkhaṁ,",
  "mil5.4.5:4.2_23": "diṭṭhibyasanampi dukkhaṁ,",
  "mil5.4.5:4.2_24": "rājabhayampi dukkhaṁ,",
  "mil5.4.5:4.2_25": "corabhayampi dukkhaṁ,",
  "mil5.4.5:4.2_26": "veribhayampi dukkhaṁ,",
  "mil5.4.5:4.2_27": "dubbhikkhabhayampi dukkhaṁ,",
  "mil5.4.5:4.2_28": "aggibhayampi dukkhaṁ,",
  "mil5.4.5:4.2_29": "udakabhayampi dukkhaṁ,",
  "mil5.4.5:4.2_30": "ūmibhayampi dukkhaṁ,",
  "mil5.4.5:4.2_31": "āvaṭṭabhayampi dukkhaṁ,",
  "mil5.4.5:4.2_32": "kumbhīlabhayampi dukkhaṁ,",
  "mil5.4.5:4.2_33": "susukābhayampi dukkhaṁ,",
  "mil5.4.5:4.2_34": "attānuvādabhayampi dukkhaṁ,",
  "mil5.4.5:4.2_35": "parānuvādabhayampi dukkhaṁ,",
  "mil5.4.5:4.2_36": "daṇḍabhayampi dukkhaṁ,",
  "mil5.4.5:4.2_37": "duggatibhayampi dukkhaṁ,",
  "mil5.4.5:4.2_38": "parisāsārajjabhayampi dukkhaṁ,",
  "mil5.4.5:4.2_39": "ājīvakabhayampi dukkhaṁ,",
  "mil5.4.5:4.2_40": "maraṇabhayampi dukkhaṁ,",
  "mil5.4.5:4.2_41": "vettehi tāḷanampi dukkhaṁ,",
  "mil5.4.5:4.2_42": "kasāhi tāḷanampi dukkhaṁ,",
  "mil5.4.5:4.2_43": "addhadaṇḍakehi tāḷanampi dukkhaṁ,",
  "mil5.4.5:4.2_44": "hatthacchedanampi dukkhaṁ,",
  "mil5.4.5:4.2_45": "pādacchedanampi dukkhaṁ,",
  "mil5.4.5:4.2_46": "hatthapādacchedanampi dukkhaṁ,",
  "mil5.4.5:4.2_47": "kaṇṇacchedanampi dukkhaṁ,",
  "mil5.4.5:4.2_48": "nāsacchedanampi dukkhaṁ,",
  "mil5.4.5:4.2_49": "kaṇṇanāsacchedanampi dukkhaṁ,",
  "mil5.4.5:4.2_50": "bilaṅgathālikampi dukkhaṁ,",
  "mil5.4.5:4.2_51": "saṅkhamuṇḍikampi dukkhaṁ,",
  "mil5.4.5:4.2_52": "rāhumukhampi dukkhaṁ,",
  "mil5.4.5:4.2_53": "jotimālikampi dukkhaṁ,",
  "mil5.4.5:4.2_54": "hatthapajjotikampi dukkhaṁ,",
  "mil5.4.5:4.2_55": "erakavattikampi dukkhaṁ,",
  "mil5.4.5:4.2_56": "cīrakavāsikampi dukkhaṁ,",
  "mil5.4.5:4.2_57": "eṇeyyakampi dukkhaṁ,",
  "mil5.4.5:4.2_58": "baḷisamaṁsikampi dukkhaṁ,",
  "mil5.4.5:4.2_59": "kahāpaṇikampi dukkhaṁ,",
  "mil5.4.5:4.2_60": "khārāpatacchikampi dukkhaṁ,",
  "mil5.4.5:4.2_61": "palighaparivattikampi dukkhaṁ,",
  "mil5.4.5:4.2_62": "palālapīṭhakampi dukkhaṁ,",
  "mil5.4.5:4.2_63": "tattena telena osiñcanampi dukkhaṁ,",
  "mil5.4.5:4.2_64": "sunakhehi khādāpanampi dukkhaṁ,",
  "mil5.4.5:4.2_65": "jīvasūlāropanampi dukkhaṁ,",
  "mil5.4.5:4.2_66": "asinā sīsacchedanampi dukkhaṁ,",
  "mil5.4.5:4.2_67": "evarūpāni, mahārāja,",
  "mil5.4.5:4.2_68": "bahuvidhāni anekavidhāni dukkhāni saṁsāragato anubhavati.",
  "mil5.4.5:5.1_0": "Yathā, mahārāja,",
  "mil5.4.5:5.1_1": "himavantapabbate abhivuṭṭhaṁ udakaṁ gaṅgāya nadiyā pāsāṇasakkharakharamarumbaāvaṭṭagaggalakaūmikavaṅkacadika-āvaraṇanīvaraṇamūlakasākhāsu",
  "mil5.4.5:5.1_2": "pariyottharati;",
  "mil5.4.5:5.2_0": "evameva kho, mahārāja, evarūpāni bahuvidhāni anekavidhāni dukkhāni saṁsāragato anubhavati.",
  "mil5.4.5:5.3_0": "Pavattaṁ, mahārāja, dukkhaṁ, appavattaṁ sukhaṁ.",
  "mil5.4.5:5.4_0": "Appavattassa guṇaṁ pavatte ca bhayaṁ dīpayamāno, mahārāja,",
  "mil5.4.5:5.4_1": "bhagavā appavattassa sacchikiriyāya jātijarābyādhimaraṇasamatikkamāya samādapesi,",
  "mil5.4.5:5.4_2": "idamettha, mahārāja, kāraṇaṁ,",
  "mil5.4.5:5.4_3": "yena kāraṇena bhagavā samādapesī”ti.",
  "mil5.4.5:5.5_0": "“Sādhu, bhante nāgasena, sunibbeṭhito pañho, sukathitaṁ kāraṇaṁ, evametaṁ tathā sampaṭicchāmī”ti.",
  "mil5.4.5:6.1_0": "Attanipātanapañho pañcamo."
}