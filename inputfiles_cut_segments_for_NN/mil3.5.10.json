{
  "mil3.5.10:0.1_0": "Milindapañha",
  "mil3.5.10:0.2_0": "Buddhavagga",
  "mil3.5.10:0.3_0": "10. Buddhanidassanapañha",
  "mil3.5.10:1.1_0": "Rājā āha—",
  "mil3.5.10:1.2_0": "“bhante nāgasena, buddho atthī”ti?",
  "mil3.5.10:1.3_0": "“Āma, mahārāja, bhagavā atthī”ti.",
  "mil3.5.10:1.4_0": "“Sakkā pana, bhante nāgasena,",
  "mil3.5.10:1.4_1": "buddho nidassetuṁ idha vā idha vā”ti?",
  "mil3.5.10:1.5_0": "“Parinibbuto, mahārāja,",
  "mil3.5.10:1.5_1": "bhagavā anupādisesāya nibbānadhātuyā,",
  "mil3.5.10:1.5_2": "na sakkā bhagavā nidassetuṁ ‘idha vā idha vā’”ti.",
  "mil3.5.10:2.1_0": "“Opammaṁ karohī”ti.",
  "mil3.5.10:2.2_0": "“Taṁ kiṁ maññasi, mahārāja,",
  "mil3.5.10:2.2_1": "mahato aggikkhandhassa jalamānassa yā acci atthaṅgatā,",
  "mil3.5.10:2.2_2": "sakkā sā acci dassetuṁ ‘idha vā idha vā’”ti?",
  "mil3.5.10:2.3_0": "“Na hi, bhante,",
  "mil3.5.10:2.3_1": "niruddhā sā acci appaññattiṁ gatā”ti.",
  "mil3.5.10:2.4_0": "“Evameva kho, mahārāja,",
  "mil3.5.10:2.4_1": "bhagavā anupādisesāya nibbānadhātuyā parinibbuto atthaṅgato,",
  "mil3.5.10:2.4_2": "na sakkā bhagavā nidassetuṁ ‘idha vā idha vā’ti, dhammakāyena pana kho, mahārāja, sakkā bhagavā nidassetuṁ.",
  "mil3.5.10:2.5_0": "Dhammo hi, mahārāja, bhagavatā desito”ti.",
  "mil3.5.10:3.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.5.10:4.1_0": "Buddhanidassanapañho dasamo.",
  "mil3.5.10:5.1_0": "Buddhavaggo pañcamo.",
  "mil3.5.10:6.1_0": "Imasmiṁ vagge dasa pañhā."
}