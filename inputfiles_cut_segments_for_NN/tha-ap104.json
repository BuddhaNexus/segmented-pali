{
  "tha-ap104:0.1_0": "Therāpadāna",
  "tha-ap104:0.2_0": "Bhikkhadāyivagga",
  "tha-ap104:0.3_0": "2 Ñāṇasaññikattheraapadāna",
  "tha-ap104:1.1_0": "“Suvaṇṇavaṇṇaṁ sambuddhaṁ,",
  "tha-ap104:1.2_0": "Nisabhājāniyaṁ yathā;",
  "tha-ap104:1.3_0": "Tidhāpabhinnaṁ mātaṅgaṁ,",
  "tha-ap104:1.4_0": "Kuñjaraṁva mahesinaṁ.",
  "tha-ap104:2.1_0": "Obhāsentaṁ disā sabbā,",
  "tha-ap104:2.2_0": "uḷurājaṁva pūritaṁ;",
  "tha-ap104:2.3_0": "Rathiyaṁ paṭipajjantaṁ,",
  "tha-ap104:2.4_0": "lokajeṭṭhaṁ apassahaṁ.",
  "tha-ap104:3.1_0": "Ñāṇe cittaṁ pasādetvā,",
  "tha-ap104:3.2_0": "paggahetvāna añjaliṁ;",
  "tha-ap104:3.3_0": "Pasannacitto sumano,",
  "tha-ap104:3.4_0": "siddhatthamabhivādayiṁ.",
  "tha-ap104:4.1_0": "Catunnavutito kappe,",
  "tha-ap104:4.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap104:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap104:4.4_0": "ñāṇasaññāyidaṁ phalaṁ.",
  "tha-ap104:5.1_0": "Tesattatimhito kappe,",
  "tha-ap104:5.2_0": "soḷasāsuṁ naruttamā;",
  "tha-ap104:5.3_0": "Sattaratanasampannā,",
  "tha-ap104:5.4_0": "cakkavattī mahabbalā.",
  "tha-ap104:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap104:6.2_0": "…pe…",
  "tha-ap104:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap104:7.1_0": "Itthaṁ sudaṁ āyasmā ñāṇasaññiko thero imā gāthāyo abhāsitthāti.",
  "tha-ap104:8.1_0": "Ñāṇasaññikattherassāpadānaṁ dutiyaṁ."
}