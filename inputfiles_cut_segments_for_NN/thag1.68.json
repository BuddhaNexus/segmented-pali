{
  "thag1.68:0.1_0": "Theragāthā",
  "thag1.68:0.2_0": "Ekakanipāta",
  "thag1.68:0.3_0": "Sattamavagga",
  "thag1.68:0.4_0": "1.68. Ekudāniyattheragāthā",
  "thag1.68:1.1_0": "“Adhicetaso appamajjato,",
  "thag1.68:1.2_0": "Munino monapathesu sikkhato;",
  "thag1.68:1.3_0": "Sokā na bhavanti tādino,",
  "thag1.68:1.4_0": "Upasantassa sadā satīmato”ti.",
  "thag1.68:1.5_0": "… Ekudāniyo thero …."
}