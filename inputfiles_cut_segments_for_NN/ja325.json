{
  "ja325:0.1_0": "Jātaka",
  "ja325:0.2_0": "Catukkanipāta",
  "ja325:0.3_0": "Kuṭidūsakavagga",
  "ja325:0.4_0": "5. Godharājajātaka",
  "ja325:1.1_0": "“Samaṇaṁ taṁ maññamāno,",
  "ja325:1.2_0": "upagacchimasaññataṁ;",
  "ja325:1.3_0": "So maṁ daṇḍena pāhāsi,",
  "ja325:1.4_0": "yathā assamaṇo tathā”.",
  "ja325:2.1_0": "“Kiṁ te jaṭāhi dummedha,",
  "ja325:2.2_0": "kiṁ te ajinasāṭiyā;",
  "ja325:2.3_0": "Abbhantaraṁ te gahanaṁ,",
  "ja325:2.4_0": "bāhiraṁ parimajjasi”.",
  "ja325:3.1_0": "“Ehi godha nivattassu,",
  "ja325:3.2_0": "bhuñja sālīnamodanaṁ;",
  "ja325:3.3_0": "Telaṁ loṇañca me atthi,",
  "ja325:3.4_0": "pahūtaṁ mayha pipphali.",
  "ja325:4.1_0": "Esa bhiyyo pavekkhāmi,",
  "ja325:4.2_0": "vammikaṁ sataporisaṁ;",
  "ja325:4.3_0": "Telaṁ loṇañca kittesi,",
  "ja325:4.4_0": "ahitaṁ mayha pipphalī”ti.",
  "ja325:5.1_0": "Godharājajātakaṁ pañcamaṁ."
}