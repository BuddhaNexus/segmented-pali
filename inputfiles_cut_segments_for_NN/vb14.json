{
  "vb14:0.1_0": "Vibhaṅga",
  "vb14:0.2_0": "Sikkhāpadavibhaṅga",
  "vb14:1.0_0": "1. Abhidhammabhājanīya",
  "vb14:1.1_0": "Pañca sikkhāpadāni—",
  "vb14:1.2_0": "pāṇātipātā veramaṇī sikkhāpadaṁ,",
  "vb14:1.2_1": "adinnādānā veramaṇī sikkhāpadaṁ,",
  "vb14:1.2_2": "kāmesumicchācārā veramaṇī sikkhāpadaṁ,",
  "vb14:1.2_3": "musāvādā veramaṇī sikkhāpadaṁ,",
  "vb14:1.2_4": "surāmerayamajjapamādaṭṭhānā veramaṇī sikkhāpadaṁ.",
  "vb14:2.1_0": "Tattha katamaṁ pāṇātipātā veramaṇī sikkhāpadaṁ?",
  "vb14:2.2_0": "Yasmiṁ samaye kāmāvacaraṁ kusalaṁ cittaṁ uppannaṁ hoti somanassasahagataṁ ñāṇasampayuttaṁ pāṇātipātā viramantassa,",
  "vb14:2.2_1": "yā tasmiṁ samaye pāṇātipātā ārati virati paṭivirati veramaṇī akiriyā akaraṇaṁ anajjhāpatti velāanatikkamo setughāto—",
  "vb14:2.3_0": "idaṁ vuccati “pāṇātipātā veramaṇī sikkhāpadaṁ”.",
  "vb14:2.4_0": "Avasesā dhammā veramaṇiyā sampayuttā.",
  "vb14:3.1_0": "Tattha katamaṁ pāṇātipātā veramaṇī sikkhāpadaṁ?",
  "vb14:3.2_0": "Yasmiṁ samaye kāmāvacaraṁ kusalaṁ cittaṁ uppannaṁ hoti somanassasahagataṁ ñāṇasampayuttaṁ pāṇātipātā viramantassa,",
  "vb14:3.2_1": "yā tasmiṁ samaye cetanā sañcetanā sañcetayitattaṁ—",
  "vb14:3.3_0": "idaṁ vuccati “pāṇātipātā veramaṇī sikkhāpadaṁ”.",
  "vb14:3.4_0": "Avasesā dhammā cetanāya sampayuttā.",
  "vb14:4.1_0": "Tattha katamaṁ pāṇātipātā veramaṇī sikkhāpadaṁ?",
  "vb14:4.2_0": "Yasmiṁ samaye kāmāvacaraṁ kusalaṁ cittaṁ uppannaṁ hoti somanassasahagataṁ ñāṇasampayuttaṁ pāṇātipātā viramantassa,",
