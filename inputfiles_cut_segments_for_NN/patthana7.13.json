{
  "patthana7.13:0.1_0": "Paṭṭhānapakaraṇa",
  "patthana7.13:0.2_0": "Dhammapaccanīya (2), Tikapaṭṭhānapāḷi (1)",
  "patthana7.13:0.3_0": "7.13. Parittārammaṇattika",
  "patthana7.13:1.1_0": "Naparittārammaṇaṁ dhammaṁ paṭicca …pe…",
  "patthana7.13:1.1_1": "namahaggatārammaṇaṁ dhammaṁ paṭicca …pe…",
  "patthana7.13:1.1_2": "naappamāṇārammaṇaṁ dhammaṁ paṭicca ….",
  "patthana7.13:1.1_3": "(Saṅkhittaṁ.)",
  "patthana7.13:2.1_0": "Hetuyā ekūnapaññāsa …pe… avigate ekūnapaññāsa."
}