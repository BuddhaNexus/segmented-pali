{
  "thag1.71:0.1_0": "Theragāthā",
  "thag1.71:0.2_0": "Ekakanipāta",
  "thag1.71:0.3_0": "Aṭṭhamavagga",
  "thag1.71:0.4_0": "1.71. Vacchapālattheragāthā",
  "thag1.71:1.1_0": "“Susukhumanipuṇatthadassinā,",
  "thag1.71:1.2_0": "Matikusalena nivātavuttinā;",
  "thag1.71:1.3_0": "Saṁsevitavuddhasīlinā,",
  "thag1.71:1.4_0": "Nibbānaṁ na hi tena dullabhan”ti.",
  "thag1.71:1.5_0": "… Vacchapālo thero …."
}