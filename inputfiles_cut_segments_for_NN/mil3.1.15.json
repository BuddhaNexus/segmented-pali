{
  "mil3.1.15:0.1_0": "Milindapañha",
  "mil3.1.15:0.2_0": "Mahāvagga",
  "mil3.1.15:0.3_0": "15. Paññālakkhaṇapañha",
  "mil3.1.15:1.1_0": "Rājā āha—",
  "mil3.1.15:1.2_0": "“bhante nāgasena, kiṁlakkhaṇā paññā”ti?",
  "mil3.1.15:1.3_0": "“Pubbeva kho, mahārāja, mayā vuttaṁ—",
  "mil3.1.15:1.4_0": "‘chedanalakkhaṇā paññā’ti, api ca obhāsanalakkhaṇā paññā”ti.",
  "mil3.1.15:1.5_0": "“Kathaṁ, bhante, obhāsanalakkhaṇā paññā”ti?",
  "mil3.1.15:1.6_0": "“Paññā, mahārāja,",
  "mil3.1.15:1.6_1": "uppajjamānā avijjandhakāraṁ vidhameti,",
  "mil3.1.15:1.6_2": "vijjobhāsaṁ janeti,",
  "mil3.1.15:1.6_3": "ñāṇālokaṁ vidaṁseti,",
  "mil3.1.15:1.6_4": "ariyasaccāni pākaṭāni karoti.",
  "mil3.1.15:1.7_0": "Tato yogāvacaro ‘aniccan’ti vā ‘dukkhan’ti vā ‘anattā’ti vā sammappaññāya passatī”ti.",
  "mil3.1.15:2.1_0": "“Opammaṁ karohī”ti.",
  "mil3.1.15:2.2_0": "“Yathā, mahārāja,",
  "mil3.1.15:2.2_1": "puriso andhakāre gehe padīpaṁ paveseyya,",
  "mil3.1.15:2.2_2": "paviṭṭho padīpo andhakāraṁ vidhameti,",
  "mil3.1.15:2.2_3": "obhāsaṁ janeti,",
  "mil3.1.15:2.2_4": "ālokaṁ vidaṁseti,",
  "mil3.1.15:2.2_5": "rūpāni pākaṭāni karoti;",
  "mil3.1.15:2.3_0": "evameva kho, mahārāja,",
  "mil3.1.15:2.3_1": "paññā uppajjamānā avijjandhakāraṁ vidhameti,",
  "mil3.1.15:2.3_2": "vijjobhāsaṁ janeti,",
  "mil3.1.15:2.3_3": "ñāṇālokaṁ vidaṁseti,",
  "mil3.1.15:2.3_4": "ariyasaccāni pākaṭāni karoti.",
  "mil3.1.15:2.4_0": "Tato yogāvacaro ‘aniccan’ti vā ‘dukkhan’ti vā ‘anattā’ti vā sammappaññāya passati.",
  "mil3.1.15:2.5_0": "Evaṁ kho, mahārāja, obhāsanalakkhaṇā paññā”ti.",
  "mil3.1.15:3.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.1.15:4.1_0": "Paññālakkhaṇapañho pannarasamo."
}