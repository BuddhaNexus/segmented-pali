{
  "ja366:0.1_0": "Jātaka",
  "ja366:0.2_0": "Pañcakanipāta",
  "ja366:0.3_0": "Vaṇṇārohavagga",
  "ja366:0.4_0": "6. Gumbiyajātaka",
  "ja366:1.1_0": "“Madhuvaṇṇaṁ madhurasaṁ,",
  "ja366:1.2_0": "madhugandhaṁ visaṁ ahu;",
  "ja366:1.3_0": "Gumbiyo ghāsamesāno,",
  "ja366:1.4_0": "araññe odahī visaṁ.",
  "ja366:2.1_0": "Madhu iti maññamānā,",
  "ja366:2.2_0": "ye taṁ visamakhādisuṁ;",
  "ja366:2.3_0": "Tesaṁ taṁ kaṭukaṁ āsi,",
  "ja366:2.4_0": "maraṇaṁ tenupāgamuṁ.",
  "ja366:3.1_0": "Ye ca kho paṭisaṅkhāya,",
  "ja366:3.2_0": "visaṁ taṁ parivajjayuṁ;",
  "ja366:3.3_0": "Te āturesu sukhitā,",
  "ja366:3.4_0": "ḍayhamānesu nibbutā.",
  "ja366:4.1_0": "Evameva manussesu,",
  "ja366:4.2_0": "visaṁ kāmā samohitā;",
  "ja366:4.3_0": "Āmisaṁ bandhanañcetaṁ,",
  "ja366:4.4_0": "maccuveso guhāsayo.",
  "ja366:5.1_0": "Evameva ime kāme,",
  "ja366:5.2_0": "āturā paricārike;",
  "ja366:5.3_0": "Ye sadā parivajjenti,",
  "ja366:5.4_0": "saṅgaṁ loke upaccagun”ti.",
  "ja366:6.1_0": "Gumbiyajātakaṁ chaṭṭhaṁ."
}