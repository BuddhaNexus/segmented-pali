{
  "ja279:0.1_0": "Jātaka",
  "ja279:0.2_0": "Tikanipāta",
  "ja279:0.3_0": "Udapānavagga",
  "ja279:0.4_0": "9. Satapattajātaka",
  "ja279:1.1_0": "“Yathā māṇavako panthe,",
  "ja279:1.2_0": "siṅgāliṁ vanagocariṁ;",
  "ja279:1.3_0": "Atthakāmaṁ pavedentiṁ,",
  "ja279:1.4_0": "anatthakāmāti maññati;",
  "ja279:1.5_0": "Anatthakāmaṁ satapattaṁ,",
  "ja279:1.6_0": "atthakāmoti maññati.",
  "ja279:2.1_0": "Evameva idhekacco,",
  "ja279:2.2_0": "puggalo hoti tādiso;",
  "ja279:2.3_0": "Hitehi vacanaṁ vutto,",
  "ja279:2.4_0": "paṭigaṇhāti vāmato.",
  "ja279:3.1_0": "Ye ca kho naṁ pasaṁsanti,",
  "ja279:3.2_0": "bhayā ukkaṁsayanti vā;",
  "ja279:3.3_0": "Tañhi so maññate mittaṁ,",
  "ja279:3.4_0": "satapattaṁva māṇavo”ti.",
  "ja279:4.1_0": "Satapattajātakaṁ navamaṁ."
}