{
  "iti25:0.1_0": "Itivuttaka 25",
  "iti25:0.2_0": "Ekakanipāta",
  "iti25:0.3_0": "Tatiyavagga",
  "iti25:0.4_0": "Musāvādasutta",
  "iti25:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti25:2.1_0": "“Ekadhammaṁ atītassa, bhikkhave,",
  "iti25:2.1_1": "purisapuggalassa nāhaṁ tassa kiñci pāpakammaṁ akaraṇīyanti vadāmi.",
  "iti25:2.2_0": "Katamaṁ ekadhammaṁ?",
  "iti25:2.3_0": "Yadidaṁ, bhikkhave, sampajānamusāvādo”ti.",
  "iti25:2.4_0": "Etamatthaṁ bhagavā avoca.",
  "iti25:2.5_0": "Tatthetaṁ iti vuccati:",
  "iti25:3.1_0": "“Ekadhammaṁ atītassa,",
  "iti25:3.2_0": "musāvādissa jantuno;",
  "iti25:3.3_0": "Vitiṇṇaparalokassa,",
  "iti25:3.4_0": "natthi pāpaṁ akāriyan”ti.",
  "iti25:4.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti25:4.2_0": "Pañcamaṁ."
}