{
  "tha-ap202:0.1_0": "Therāpadāna",
  "tha-ap202:0.2_0": "Tamālapupphiyavagga",
  "tha-ap202:0.3_0": "10 Yūthikapupphiyattheraapadāna",
  "tha-ap202:1.1_0": "“Padumuttaro nāma jino,",
  "tha-ap202:1.2_0": "āhutīnaṁ paṭiggaho;",
  "tha-ap202:1.3_0": "Pavanā nikkhamitvāna,",
  "tha-ap202:1.4_0": "vihāraṁ yāti cakkhumā.",
  "tha-ap202:2.1_0": "Ubho hatthehi paggayha,",
  "tha-ap202:2.2_0": "yūthikaṁ pupphamuttamaṁ;",
  "tha-ap202:2.3_0": "Buddhassa abhiropayiṁ,",
  "tha-ap202:2.4_0": "mettacittassa tādino.",
  "tha-ap202:3.1_0": "Tena cittappasādena,",
  "tha-ap202:3.2_0": "anubhotvāna sampadā;",
  "tha-ap202:3.3_0": "Kappānaṁ satasahassaṁ,",
  "tha-ap202:3.4_0": "duggatiṁ nupapajjahaṁ.",
  "tha-ap202:4.1_0": "Ito paññāsakappesu,",
  "tha-ap202:4.2_0": "eko āsiṁ janādhipo;",
  "tha-ap202:4.3_0": "Samittanandano nāma,",
  "tha-ap202:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap202:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap202:5.2_0": "…pe…",
  "tha-ap202:5.3_0": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap202:6.1_0": "Itthaṁ sudaṁ āyasmā yūthikapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap202:7.1_0": "Yūthikapupphiyattherassāpadānaṁ dasamaṁ.",
  "tha-ap202:7.2_0": "Tamālapupphiyavaggo vīsatimo.",
  "tha-ap202:8.1_0": "Tassuddānaṁ",
  "tha-ap202:9.1_0": "Tamālatiṇasanthāro,",
  "tha-ap202:9.2_0": "khaṇḍaphulli asokiyo;",
  "tha-ap202:9.3_0": "Aṅkolakī kisalayo,",
  "tha-ap202:9.4_0": "tinduko nelapupphiyo;",
  "tha-ap202:9.5_0": "Kiṅkaṇiko yūthiko ca,",
  "tha-ap202:9.6_0": "gāthā paññāsa aṭṭha cāti.",
  "tha-ap202:10.1_0": "Atha vagguddānaṁ",
  "tha-ap202:11.1_0": "Bhikkhādāyī parivāro,",
  "tha-ap202:11.2_0": "sereyyo sobhito tathā;",
  "tha-ap202:11.3_0": "Chattañca bandhujīvī ca,",
  "tha-ap202:11.4_0": "supāricariyopi ca.",
  "tha-ap202:12.1_0": "Kumudo kuṭajo ceva,",
  "tha-ap202:12.2_0": "tamāli dasamo kato;",
  "tha-ap202:12.3_0": "Chasatāni ca gāthāni,",
  "tha-ap202:12.4_0": "chasaṭṭhi ca tatuttari.",
  "tha-ap202:13.1_0": "Bhikkhāvaggadasakaṁ.",
  "tha-ap202:14.1_0": "Dutiyasatakaṁ samattaṁ."
}