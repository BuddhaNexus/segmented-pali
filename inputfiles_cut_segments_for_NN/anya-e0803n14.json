{
  "anya-e0803n14:0_0": "14. Atthattikavibhāga",
  "anya-e0803n14:1_0": "Bhūdhātu tāya nipphanna-rūpañcāti idaṁ dvayaṁ;",
  "anya-e0803n14:2_0": "Katvā padhānamamhehi, sabbametaṁ papañcitaṁ.",
  "anya-e0803n14:3_0": "Bhavatissa vasā dāni, vakkhāmatthattikaṁ varaṁ;",
  "anya-e0803n14:4_0": "Atthuddhāro tumantañca, tvādiyantaṁ tikaṁ idha.",
  "anya-e0803n14:5_0": "Tasmā tāva bhūdhātuto pavattassa bhūtasaddassa atthuddhāro nīyate –",
  "anya-e0803n14:6_0": "Khandhasattāmanussesu, vijjamāne ca dhātuyaṁ;",
  "anya-e0803n14:7_0": "Khīṇāsave rukkhādimhi, bhūtasaddo pavattati.",
  "anya-e0803n14:8_0": "Uppāde cāpi viññeyyo, bhūtasaddo vibhāvinā;",
  "anya-e0803n14:9_0": "Vipule sopasaggoyaṁ, hīḷane vidhamepi ca;",
  "anya-e0803n14:10_0": "Parājaye vediyane, nāme pākaṭatāya ca.",
  "anya-e0803n14:11_0": "Vuttañhetaṁ –",
  "anya-e0803n14:11_1": "bhūtasaddo pañcakkhandhāmanussadhātuvijjamānakhīṇāsavasattarukkhādīsu dissati.",
  "anya-e0803n14:11_2": "‘‘Bhūtamidanti bhikkhave samanupassathā’’tiādīsu hi ayaṁ pañcakkhandhesu dissati.",
  "anya-e0803n14:11_3": "‘‘Yānīdha bhūtāni samāgatānī’’ti ettha amanusse.",
  "anya-e0803n14:11_4": "‘‘Cattāro kho bhikkhu mahābhūtā hetū’’ti ettha dhātūsu.",
  "anya-e0803n14:11_5": "‘‘Bhūtasmiṁ pācittiya’’ntiādīsu vijjamāne.",
  "anya-e0803n14:11_6": "‘‘Yo ca kālaghaso bhūto’’ti ettha khīṇāsave.",
  "anya-e0803n14:11_7": "‘‘Sabbeva nikkhipissanti,",
  "anya-e0803n14:11_8": "bhūtā loke samussaya’’nti ettha satte.",
  "anya-e0803n14:11_9": "‘‘Bhūtagāmapātabyatāyā’’ti ettha rukkhādīsūti.",
  "anya-e0803n14:11_10": "Mūlapariyāyasuttaṭṭhakathāya vacanaṁ idaṁ.",
  "anya-e0803n14:11_11": "Ṭīkāyamādisaddena uppādādīni gayhare.",
  "anya-e0803n14:11_12": "Vuttañhetaṁ –",
  "anya-e0803n14:11_13": "‘‘jātaṁ bhūtaṁ saṅkhata’’ntiādīsu bhūtasaddo uppāde dissati.",
  "anya-e0803n14:11_14": "Saupasaggo pana ‘‘pabhūtamariyo pakaroti puñña’’ntiādīsu vipule.",
  "anya-e0803n14:11_15": "‘‘Yebhuyyena bhikkhūnaṁ paribhūtarūpo’’tiādīsu hīḷane.",
  "anya-e0803n14:11_16": "‘‘Sambhūto sāṇavāsī’’tiādīsu paññattiyaṁ.",
  "anya-e0803n14:11_17": "‘‘Abhibhūto māro vijito saṅgāmo’’tiādīsu vidhamane.",
  "anya-e0803n14:11_18": "‘‘Parābhūtarūpokho ayaṁ acelo pāthikaputto’’tiādīsu parājaye.",
  "anya-e0803n14:11_19": "‘‘Anubhūtaṁ sukhadukkha’’ntiādīsu vediyane.",
  "anya-e0803n14:11_20": "‘‘Vibhūtaṁ paññāyā’’tiādīsu pākaṭīkaraṇe dissati,",
  "anya-e0803n14:11_21": "te sabbe ‘‘rukkhādīsū’’tiādisaddena saṅgahitāti daṭṭhabbāti.",
  "anya-e0803n14:12_0": "Idāni tumantapadāni vuccante –",
  "anya-e0803n14:13_0": "Bhavituṁ,",
  "anya-e0803n14:13_1": "ubbhavituṁ,",
  "anya-e0803n14:13_2": "samubbhavituṁ,",
  "anya-e0803n14:13_3": "pabhavituṁ,",
  "anya-e0803n14:13_4": "parābhavituṁ,",
  "anya-e0803n14:13_5": "atibhavituṁ,",
  "anya-e0803n14:13_6": "sambhavituṁ,",
  "anya-e0803n14:13_7": "vibhavituṁ, bhotuṁ,",
  "anya-e0803n14:13_8": "sambhotuṁ, vibhotuṁ,",
  "anya-e0803n14:13_9": "pātubhavituṁ,",
  "anya-e0803n14:13_10": "pātubbhavituṁ vā,",
  "anya-e0803n14:13_11": "pātubhotuṁ.",
  "anya-e0803n14:13_12": "Imāni akammakāni tumantapadāni.",
  "anya-e0803n14:14_0": "Paribhotuṁ paribhavituṁ,",
  "anya-e0803n14:14_1": "abhibhotuṁ abhibhavituṁ,",
  "anya-e0803n14:14_2": "adhibhotuṁ adhibhavituṁ,",
  "anya-e0803n14:14_3": "atibhotuṁ atibhavituṁ,",
  "anya-e0803n14:14_4": "anubhotuṁ anubhavituṁ,",
  "anya-e0803n14:14_5": "samanubhotuṁ samanubhavituṁ,",
  "anya-e0803n14:14_6": "abhisambhotuṁ abhisambhavituṁ.",
  "anya-e0803n14:14_7": "Imāni sakammakāni tumantapadāni,",
  "anya-e0803n14:14_8": "sabbānetāni suddhakattari bhavanti.",
  "anya-e0803n14:15_0": "‘‘Bhāvetuṁ,",
  "anya-e0803n14:15_1": "pabhāvetuṁ,",
  "anya-e0803n14:15_2": "sambhāvetuṁ,",
  "anya-e0803n14:15_3": "vibhāvetuṁ,",
  "anya-e0803n14:15_4": "paribhāvetuṁ’’iccevamādīni hetukattari tumantapadāni,",
  "anya-e0803n14:15_5": "sabbānipi hetukattari tumantapadāni sakammakāniyeva bhavanti.",
  "anya-e0803n14:15_6": "Uddesoyaṁ.",
  "anya-e0803n14:16_0": "Tatra samānatthapadesu ekamevādipadaṁ gahetvā niddeso kātabbo –",
  "anya-e0803n14:16_1": "bhavitunti hotuṁ vijjituṁ paññāyituṁ sarūpaṁ labhituṁ.",
  "anya-e0803n14:16_2": "Ettha vuttanayānusārena sesānampi tumantānaṁ niddeso vitthāretabbo,",
  "anya-e0803n14:16_3": "sabbāni tumantapadāni catutthiyatthe vattanti ‘‘tvaṁ mama cittamaññāya,",
  "anya-e0803n14:16_4": "nettaṁ yācitumāgato’’ti ettha viya.",
  "anya-e0803n14:16_5": "Yācitunti hi yācanatthāyāti attho.",
  "anya-e0803n14:16_6": "Tasmā bhavituntiādīnampi ‘‘bhavanatthāyā’’ti vā ‘‘bhavanattha’’nti vā ‘‘bhavanāyā’’ti vā ādinā attho gahetabbo.",
  "anya-e0803n14:16_7": "Apica ‘‘nekkhammaṁ daṭṭhu khemato’’ti ettha ‘‘daṭṭhu’’nti padassa ‘‘disvā’’ti atthadassanato yathārahaṁ",
  "anya-e0803n14:16_8": "tumantāni tvāsaddantapadatthavasenapi gahetabbāni.",
  "anya-e0803n14:16_9": "Etāni ca nipātapadesu saṅgahaṁgacchanti.",
  "anya-e0803n14:16_10": "Vuttañhi niruttipiṭake nipātapadaparicchede ‘‘tuṁ iti catutthiyā’’ti.",
  "anya-e0803n14:16_11": "Tatrāyamattho ‘‘tuṁ iti etadanto nipāto catutthiyā atthe vattatī’’ti.",
  "anya-e0803n14:17_0": "Tumantakathā samattā.",
  "anya-e0803n14:18_0": "Idāni tvādiyantapadāni vuccante –",
  "anya-e0803n14:19_0": "Bhavitvā,",
  "anya-e0803n14:19_1": "bhavitvāna,",
  "anya-e0803n14:19_2": "bhavituna, bhaviya,",
  "anya-e0803n14:19_3": "bhaviyāna.",
  "anya-e0803n14:19_4": "Ubbhavitvā,",
  "anya-e0803n14:19_5": "ubbhavitvāna,",
  "anya-e0803n14:19_6": "ubbhavituna,",
  "anya-e0803n14:19_7": "ubbhaviya,",
  "anya-e0803n14:19_8": "ubbhaviyāna.",
  "anya-e0803n14:19_9": "Esa nayo ‘‘samubbhavitvā,",
  "anya-e0803n14:19_10": "parābhavitvā,",
  "anya-e0803n14:19_11": "sambhavitvā,",
  "anya-e0803n14:19_12": "vibhavitvā,",
  "anya-e0803n14:19_13": "pātubbhavitvā’’ti etthāpi.",
  "anya-e0803n14:19_14": "Imāni akammakāni ussukkanatthāni tvādiyantapadāni.",
  "anya-e0803n14:20_0": "Bhutvā, bhutvāna,",
  "anya-e0803n14:20_1": "paribhavitvā,",
  "anya-e0803n14:20_2": "paribhavitvāna,",
  "anya-e0803n14:20_3": "paribhavituna,",
  "anya-e0803n14:20_4": "paribhaviya,",
  "anya-e0803n14:20_5": "paribhaviyāna,",
  "anya-e0803n14:20_6": "paribhuyya.",
  "anya-e0803n14:20_7": "Abhibhavitvā,",
  "anya-e0803n14:20_8": "abhibhavitvāna,",
  "anya-e0803n14:20_9": "abhibhavituna,",
  "anya-e0803n14:20_10": "abhibhaviya,",
  "anya-e0803n14:20_11": "abhibhaviyāna,",
  "anya-e0803n14:20_12": "abhibhuyya.",
  "anya-e0803n14:20_13": "Esa nayo ‘‘adhibhavitvā,",
  "anya-e0803n14:20_14": "atibhavitvā,",
  "anya-e0803n14:20_15": "anubhavitvā’’ti etthāpi.",
  "anya-e0803n14:20_16": "Idañcettha nidassanaṁ.",
  "anya-e0803n14:20_17": "‘‘Tamavoca rājā anubhaviyāna tampi,",
  "anya-e0803n14:20_18": "eyyāsi khippaṁ ahamapi pūjaṁ kassa’’nti.",
  "anya-e0803n14:20_19": "Anubhutvā,",
  "anya-e0803n14:20_20": "anubhutvāna.",
  "anya-e0803n14:20_21": "Adhibhotvā,",
  "anya-e0803n14:20_22": "adhibhotvāna.",
  "anya-e0803n14:21_0": "‘‘Saṭṭhi kappasahassāni, devaloke ramissati;",
  "anya-e0803n14:22_0": "Aññe deve adhibhotvā, issaraṁ kārayissatī’’ti",
  "anya-e0803n14:23_0": "Idamettha pāḷinidassanaṁ,",
  "anya-e0803n14:23_1": "imāni sakammakāni ussukkanatthāni tvādiyantapadāni.",
  "anya-e0803n14:23_2": "Imāni cattāri suddhakattariyeva bhavanti.",
  "anya-e0803n14:24_0": "‘‘Bhāvetvā,",
  "anya-e0803n14:24_1": "bhāvetvāna.",
  "anya-e0803n14:24_2": "Pabhāvetvā,",
  "anya-e0803n14:24_3": "pabhāvetvāna.",
  "anya-e0803n14:24_4": "Sambhāvetvā,",
  "anya-e0803n14:24_5": "sambhāvetvāna.",
  "anya-e0803n14:24_6": "Vibhāvetvā,",
  "anya-e0803n14:24_7": "vibhāvetvāna.",
  "anya-e0803n14:24_8": "Paribhāvetvā,",
  "anya-e0803n14:24_9": "paribhāvetvāna’’iccevamādīni sakammakāni ussukkanatthāni tvādiyantapadāni hetukattariyeva bhavanti.",
  "anya-e0803n14:24_10": "Uddesoyaṁ.",
  "anya-e0803n14:25_0": "Tatra samānatthapadesu ekamevādipadaṁ gahetvā niddeso kātabbo –",
  "anya-e0803n14:25_1": "bhavitvāti hutvā paññāyitvā sarūpaṁ labhitvā.",
  "anya-e0803n14:25_2": "Evaṁ vuttanayānusārena sesānampi tvādiyantapadānaṁ niddeso vitthāretabbo.",
  "anya-e0803n14:25_3": "Ayaṁ pana viseso bhutvāti sampattiṁ anubhutvāti sakammakavasena attho gahetabbo.",
  "anya-e0803n14:25_4": "Bhutvā anubhutvāti imesañhi samānatthataṁ saddhammavidū icchanti.",
  "anya-e0803n14:25_5": "Atridaṁ vuccati –",
  "anya-e0803n14:26_0": "‘‘Bhutvā bhutvāna’’iccete, ‘‘anubhutvā’’timassa hi;",
  "anya-e0803n14:27_0": "Atthaṁ sūcenti ‘‘hutvā’’ti, padassa pana neva te.",
  "anya-e0803n14:28_0": "Keci ‘‘bhūtvā’’ti dīghattaṁ, tassa icchanti sāsane;",
  "anya-e0803n14:29_0": "Dīghatā rassatā ceva, dvayampetaṁ padissati.",
  "anya-e0803n14:30_0": "Saddasatthe ca ‘‘bhūtvā’’ti, dīghattasañhitaṁ padaṁ;",
  "anya-e0803n14:31_0": "‘‘Bhavitvā’’ti padassatthaṁ, dīpeti na tu sāsane.",
  "anya-e0803n14:32_0": "‘‘Hutvā’’iti padaṁyeva, dīpeti jinasāsane;",
  "anya-e0803n14:33_0": "‘‘Bhavitvā’’ti padassatthaṁ, natthi aññattha taṁ padaṁ.",
  "anya-e0803n14:34_0": "Iccevaṁ savisesantu, vacanaṁ sāradassinā;",
  "anya-e0803n14:35_0": "Sāsane saddasatthe ca, viññunā pekkhitabbakaṁ.",
  "anya-e0803n14:36_0": "Evaṁ ussukkanatthe pavattāni tvādiyantapadānipi niddiṭṭhāni,",
  "anya-e0803n14:36_1": "sabbānetāni avibhattikānīti gahetabbāni.",
  "anya-e0803n14:36_2": "Niruttipiṭake hi nipātaparicchede avibhattikāni katvā tvādiyantapadāni vuttāni.",
  "anya-e0803n14:36_3": "Saddatthavidūnaṁ pana mate paṭhamādivibhattivasena savibhattikāni bhavanti.",
  "anya-e0803n14:37_0": "Imasmiñca pana tvādiyantādhikāre idañcupalakkhitabbaṁ –",
  "anya-e0803n14:37_1": "bhutvā gacchati,",
  "anya-e0803n14:37_2": "bhutvā gato,",
  "anya-e0803n14:37_3": "bhutvā gamissasi,",
  "anya-e0803n14:37_4": "kasitvā vapati.",
  "anya-e0803n14:37_5": "Umaṅgā nikkhamitvāna,",
  "anya-e0803n14:37_6": "vedeho nāvamāruhi.",
  "anya-e0803n14:37_7": "Bhutvāna bhikkhu bhikkhassu iccādī samānakattukānaṁ dhātūnaṁ pubbakāle tvādisaddappayogā.",
  "anya-e0803n14:37_8": "‘‘Bhutvā gacchatī’’ti ettha hi ‘‘bhutvā’’ti idaṁ pubbakālakriyādīpakaṁ padaṁ.",
  "anya-e0803n14:37_9": "‘‘Gacchatī’’ti idaṁ pana uttarakālakriyādīpakaṁ,",
  "anya-e0803n14:37_10": "samānakattukāni cetāni padāni ekakattukānaṁ kriyānaṁ vācakattā.",
  "anya-e0803n14:37_11": "Tathā hettha yo gamanakriyāya kattā,",
  "anya-e0803n14:37_12": "so eva bhuñjanakriyāya kattubhūto daṭṭhabbo.",
  "anya-e0803n14:37_13": "Ayaṁ nayo aññatrāpi īdisesu ṭhānesu netabbo.",
  "anya-e0803n14:38_0": "‘‘Andhakāraṁ nihantvāna, uditoyaṁ divākaro;",
  "anya-e0803n14:39_0": "Vaṇṇaṁ paññāvabhāsehi, obhāsetvā samuggato’’",
  "anya-e0803n14:40_0": "Iccādīnipi pana samānakattukānaṁ samānakāle tvādisaddappayogā.",
  "anya-e0803n14:40_1": "Ettha hi ‘‘nihantvānā’’ti padaṁ samānakālakriyādīpakaṁ padaṁ.",
  "anya-e0803n14:40_2": "‘‘Udito’’ti idaṁ pana uttarakālakriyādīpakaṁ padanti na vattabbaṁ samānakālakriyāya idhādhippetattā.",
  "anya-e0803n14:40_3": "Tasmāyeva samānakālakriyādīpakaṁ padanti gahetabbaṁ.",
  "anya-e0803n14:40_4": "Ayaṁ nayo aññatrāpi īdisesu ṭhānesu netabbo.",
  "anya-e0803n14:41_0": "Keci pana ‘‘mukhaṁ byādāya sayati,",
  "anya-e0803n14:41_1": "akkhiṁ parivattetvā passatī’’ti udāharanti.",
  "anya-e0803n14:41_2": "Apare ‘‘nisajja adhīte,",
  "anya-e0803n14:41_3": "ṭhatvā kathetī’’ti.",
  "anya-e0803n14:41_4": "Tattha byādānaparivattanuttarakālo byādānūpasamalakkhaṇaṁ passanakriyāya lakkhiyati.",
  "anya-e0803n14:41_5": "‘‘Nisajja adhīte,",
  "anya-e0803n14:41_6": "ṭhatvā kathetī’’ti ca samānakālatāyapi ajjhenakathanehi pubbepi nisajjaṭṭhānāni hontīti sakkā pubbuttarakālatā sambhāvetuṁ,",
  "anya-e0803n14:41_7": "tasmā purimāniyeva udāharaṇāni yuttāni.",
  "anya-e0803n14:41_8": "Udayasamakālameva hi tannivattanīyanivattananti.",
  "anya-e0803n14:42_0": "‘‘Dvāramāvaritvā pavisati’’iccādi samānakattukānaṁ aparakāle tvādisaddappayogo.",
  "anya-e0803n14:42_1": "Yasmā panettha pavisanakriyā purimā,",
  "anya-e0803n14:42_2": "āvaraṇakriyā pana pacchimā,",
  "anya-e0803n14:42_3": "tasmā ‘‘āvaritvā’’ti idaṁ aparakālakriyādīpakaṁ padanti veditabbaṁ.",
  "anya-e0803n14:42_4": "‘‘Pavisatī’’ti idaṁ pana pubbakālakriyādīpakaṁ padanti.",
  "anya-e0803n14:42_5": "Ayaṁ nayo aññatrāpi īdisesu ṭhānesu netabbo.",
  "anya-e0803n14:42_6": "Apare ‘‘dha’nti kacca patito daṇḍo’’ti udāharaṇanti.",
  "anya-e0803n14:42_7": "Abhighātabhūtasamāyoge pana abhighātajasaddassa samānakālatā ettha labbhatīti idhāpi purimāniyeva udāharaṇāni yuttānīti.",
  "anya-e0803n14:43_0": "‘‘Pisācaṁ disvā cassa bhayaṁ hoti.",
  "anya-e0803n14:43_1": "Paññāya cassa disvā āsavā parikkhīṇā’’iccādi asamāne kattari payogo.",
  "anya-e0803n14:43_2": "Ettha hi pisācaṁ disvā purisassa bhayaṁ hoti,",
  "anya-e0803n14:43_3": "paññāya disvā assa puggalassa āsavā parikkhīṇā.",
  "anya-e0803n14:43_4": "Evaṁ samānakattukatā dhātūnaṁ na labbhati dassanakriyāya purisesu pavattanato,",
  "anya-e0803n14:43_5": "bhavanādikriyāya ca bhayādīsu pavattanatoti daṭṭhabbaṁ.",
  "anya-e0803n14:43_6": "Ayaṁ nayo aññatrāpi īdisesu ṭhānesu netabbo.",
  "anya-e0803n14:44_0": "Idampi panettha upalakkhitabbaṁ ‘‘appatvā nadiṁ pabbato,",
  "anya-e0803n14:44_1": "atikkamma pabbataṁ nadī’’iccādi parāparayogo.",
  "anya-e0803n14:44_2": "‘‘Sīhaṁ disvā bhayaṁ hoti,",
  "anya-e0803n14:44_3": "ghataṁ pivitvā balaṁ jāyate,",
  "anya-e0803n14:44_4": "‘dha’nti katvā daṇḍo patito’’iccādi lakkhaṇahetuādippayogo.",
  "anya-e0803n14:44_5": "‘‘Nhatvā gamanaṁ,",
  "anya-e0803n14:44_6": "bhutvā sayanaṁ.",
  "anya-e0803n14:44_7": "Upādāya rūpa’’miccāpi byattayena saddasiddhippayogoti.",
  "anya-e0803n14:45_0": "Iccevaṁ sabbathāpi samānakattukānaṁ dhātūnaṁ pubbakāle tvādisaddappayogo,",
  "anya-e0803n14:45_1": "samānakattukānaṁ samānakāle tvādisaddappayogo,",
  "anya-e0803n14:45_2": "samānakattukānaṁ aparakāle tvādisaddappayogo,",
  "anya-e0803n14:45_3": "asamānakattukānaṁ tvādisaddappayogo,",
  "anya-e0803n14:45_4": "parāparayogo ,",
  "anya-e0803n14:45_5": "lakkhaṇahetuādippayogo,",
  "anya-e0803n14:45_6": "byattayena saddasiddhippayogoti sattadhā tvādiyantānaṁ padānaṁ payogo veditabbo.",
  "anya-e0803n14:46_0": "Yadi evaṁ kasmā kaccāyane ‘‘pubbakālekakattukānaṁ tuna tvāna tvā vā’’ti pubbakāleyeva ekakattukaggahaṇaṁ katanti?",
  "anya-e0803n14:46_1": "Yetuyyena tvādiyantānaṁ padānaṁ purimakālakriyādīpanato.",
  "anya-e0803n14:46_2": "Kaccāyane hi yebhuyyena pavattiṁ sandhāya ‘‘pubbakālekakattukāna’’nti vuttaṁ.",
  "anya-e0803n14:46_3": "Yasmā pana ‘‘iti katvā’’tiādīnaṁ padānaṁ hetuatthavasenapi pubbācariyehi attho saṁvaṇṇito,",
  "anya-e0803n14:46_4": "tasmā ‘‘bhavitvā’’tiādīnaṁ bhūdhātumayānaṁ tvādisaddantānaṁ padānaṁ aññesañca ‘‘pacitvā’’tiādīnaṁ yathāpayogaṁ",
  "anya-e0803n14:46_5": "‘‘bhavanahetu pacanahetū’’tiādinā hetuatthopi gahetabbo.",
  "anya-e0803n14:46_6": "Atridaṁ vuccati –",
  "anya-e0803n14:47_0": "Hetutthepi yato honti, saddā ussukkanatthakā;",
  "anya-e0803n14:48_0": "Tasmā hetuvasenāpi, vadeyyatthaṁ vicakkhaṇo.",
  "anya-e0803n14:49_0": "‘‘Iti katvā’’ti saddassa, atthasaṁvaṇṇanāsu hi;",
  "anya-e0803n14:50_0": "‘‘Iti karaṇahetū’’ti, attho dhīrehi gayhati.",
  "anya-e0803n14:51_0": "‘‘Gacchāmi dāni nibbānaṁ, yattha gantvā na socati’’;",
  "anya-e0803n14:52_0": "Iti pāṭhepi hetuttho, gayhate pubbaviññubhi.",
  "anya-e0803n14:53_0": "‘‘Yasmiṁ nibbāne gamana-hetū’’ti hi kathīyate;",
  "anya-e0803n14:54_0": "Hetutthevaṁ yathāyoga-maññatrāpi ayaṁ nayo.",
  "anya-e0803n14:55_0": "Evaṁ bhūtasaddassa atthuddhāro ca tumantapadañca tvādiyantapadañcāti atthattikaṁ vibhattaṁ.",
  "anya-e0803n14:56_0": "Yo imamatthatikaṁ suvibhattaṁ,",
  "anya-e0803n14:57_0": "Kaṇṇarasāyanamāgamikānaṁ;",
  "anya-e0803n14:58_0": "Dhārayate sa bhave gatakaṅkhā,",
  "anya-e0803n14:59_0": "Pāvacanamhi gate sukhumatthe.",
  "anya-e0803n14:60_0": "Iti navaṅge sāṭṭhakathe piṭakattaye byappathagatīsu viññūnaṁ",
  "anya-e0803n14:61_0": "Kosallatthāya kate saddanītippakaraṇe",
  "anya-e0803n14:62_0": "Atthattikavibhāgo nāma",
  "anya-e0803n14:63_0": "Cuddasamo paricchedo.",
  "anya-e0803n14:64_0": "Evaṁ nānappakārato bhūdhāturūpāni dassitāni.",
  "anya-e0803n14:65_0": "Padamālā niṭṭhitā."
}