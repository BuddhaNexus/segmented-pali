{
  "bv21:0.1_0": "Buddhavaṁsa",
  "bv21:0.2_0": "21 Vipassībuddhavaṁsa",
  "bv21:1.1_0": "“Phussassa ca aparena,",
  "bv21:1.2_0": "sambuddho dvipaduttamo;",
  "bv21:1.3_0": "Vipassī nāma nāmena,",
  "bv21:1.4_0": "loke uppajji cakkhumā.",
  "bv21:2.1_0": "Avijjaṁ sabbaṁ padāletvā,",
  "bv21:2.2_0": "patto sambodhimuttamaṁ;",
  "bv21:2.3_0": "Dhammacakkaṁ pavattetuṁ,",
  "bv21:2.4_0": "pakkāmi bandhumatīpuraṁ.",
  "bv21:3.1_0": "Dhammacakkaṁ pavattetvā,",
  "bv21:3.2_0": "ubho bodhesi nāyako;",
  "bv21:3.3_0": "Gaṇanāya na vattabbo,",
  "bv21:3.4_0": "paṭhamābhisamayo ahu.",
  "bv21:4.1_0": "Punāparaṁ amitayaso,",
  "bv21:4.2_0": "tattha saccaṁ pakāsayi;",
  "bv21:4.3_0": "Caturāsītisahassānaṁ,",
  "bv21:4.4_0": "dutiyābhisamayo ahu.",
  "bv21:5.1_0": "Caturāsītisahassāni,",
  "bv21:5.2_0": "sambuddhaṁ anupabbajuṁ;",
  "bv21:5.3_0": "Tesamārāmapattānaṁ,",
  "bv21:5.4_0": "dhammaṁ desesi cakkhumā.",
  "bv21:6.1_0": "Sabbākārena bhāsato,",
  "bv21:6.2_0": "sutvā upanisādino;",
  "bv21:6.3_0": "Tepi dhammavaraṁ gantvā,",
  "bv21:6.4_0": "tatiyābhisamayo ahu.",
  "bv21:7.1_0": "Sannipātā tayo āsuṁ,",
  "bv21:7.2_0": "vipassissa mahesino;",
  "bv21:7.3_0": "Khīṇāsavānaṁ vimalānaṁ,",
  "bv21:7.4_0": "santacittāna tādinaṁ.",
  "bv21:8.1_0": "Aṭṭhasaṭṭhisatasahassānaṁ,",
  "bv21:8.2_0": "Paṭhamo āsi samāgamo;",
  "bv21:8.3_0": "Bhikkhusatasahassānaṁ,",
  "bv21:8.4_0": "Dutiyo āsi samāgamo.",
  "bv21:9.1_0": "Asītibhikkhusahassānaṁ,",
  "bv21:9.2_0": "tatiyo āsi samāgamo;",
  "bv21:9.3_0": "Tattha bhikkhugaṇamajjhe,",
  "bv21:9.4_0": "sambuddho atirocati.",
  "bv21:10.1_0": "Ahaṁ tena samayena,",
  "bv21:10.2_0": "nāgarājā mahiddhiko;",
  "bv21:10.3_0": "Atulo nāma nāmena,",
  "bv21:10.4_0": "puññavanto jutindharo.",
  "bv21:11.1_0": "Nekānaṁ nāgakoṭīnaṁ,",
  "bv21:11.2_0": "parivāretvānahaṁ tadā;",
  "bv21:11.3_0": "Vajjanto dibbaturiyehi,",
  "bv21:11.4_0": "lokajeṭṭhaṁ upāgamiṁ.",
  "bv21:12.1_0": "Upasaṅkamitvā sambuddhaṁ,",
  "bv21:12.2_0": "Vipassiṁ lokanāyakaṁ;",
  "bv21:12.3_0": "Maṇimuttaratanakhacitaṁ,",
  "bv21:12.4_0": "Sabbābharaṇavibhūsitaṁ;",
  "bv21:12.5_0": "Nimantetvā dhammarājassa,",
  "bv21:12.6_0": "Suvaṇṇapīṭhamadāsahaṁ.",
  "bv21:13.1_0": "Sopi maṁ buddho byākāsi,",
  "bv21:13.2_0": "saṅghamajjhe nisīdiya;",
  "bv21:13.3_0": "‘Ekanavutito kappe,",
  "bv21:13.4_0": "ayaṁ buddho bhavissati.",
  "bv21:14.1_0": "Ahu kapilavhayā rammā,",
  "bv21:14.2_0": "nikkhamitvā tathāgato;",
  "bv21:14.3_0": "Padhānaṁ padahitvāna,",
  "bv21:14.4_0": "katvā dukkarakārikaṁ.",
  "bv21:15.1_0": "Ajapālarukkhamūlasmiṁ,",
  "bv21:15.2_0": "nisīditvā tathāgato;",
  "bv21:15.3_0": "Tattha pāyāsaṁ paggayha,",
  "bv21:15.4_0": "nerañjaramupehiti.",
  "bv21:16.1_0": "Nerañjarāya tīramhi,",
  "bv21:16.2_0": "pāyāsaṁ ada so jino;",
  "bv21:16.3_0": "Paṭiyattavaramaggena,",
  "bv21:16.4_0": "bodhimūlamupehiti.",
  "bv21:17.1_0": "Tato padakkhiṇaṁ katvā,",
  "bv21:17.2_0": "bodhimaṇḍaṁ anuttaro;",
  "bv21:17.3_0": "Assatthamūle sambodhiṁ,",
  "bv21:17.4_0": "bujjhissati mahāyaso.",
  "bv21:18.1_0": "Imassa janikā mātā,",
  "bv21:18.2_0": "māyā nāma bhavissati;",
  "bv21:18.3_0": "Pitā suddhodano nāma,",
  "bv21:18.4_0": "ayaṁ hessati gotamo.",
  "bv21:19.1_0": "Anāsavā vītarāgā,",
  "bv21:19.2_0": "santacittā samāhitā;",
  "bv21:19.3_0": "Kolito upatisso ca,",
  "bv21:19.4_0": "aggā hessanti sāvakā;",
  "bv21:19.5_0": "Ānando nāmupaṭṭhāko,",
  "bv21:19.6_0": "upaṭṭhissatimaṁ jinaṁ.",
  "bv21:20.1_0": "Khemā uppalavaṇṇā ca,",
  "bv21:20.2_0": "aggā hessanti sāvikā;",
  "bv21:20.3_0": "Anāsavā vītarāgā,",
  "bv21:20.4_0": "santacittā samāhitā;",
  "bv21:20.5_0": "Bodhi tassa bhagavato,",
  "bv21:20.6_0": "assatthoti pavuccati.",
  "bv21:21.1_0": "Citto ca hatthāḷavako,",
  "bv21:21.2_0": "aggā hessantupaṭṭhakā;",
  "bv21:21.3_0": "Nandamātā ca uttarā,",
  "bv21:21.4_0": "aggā hessantupaṭṭhikā;",
  "bv21:21.5_0": "Āyu vassasataṁ tassa,",
  "bv21:21.6_0": "gotamassa yasassino.",
  "bv21:22.1_0": "Idaṁ sutvāna vacanaṁ,",
  "bv21:22.2_0": "…pe…",
  "bv21:22.3_0": "hessāma sammukhā imaṁ’.",
  "bv21:23.1_0": "Tassāhaṁ vacanaṁ sutvā,",
  "bv21:23.2_0": "bhiyyo cittaṁ pasādayiṁ;",
  "bv21:23.3_0": "Uttariṁ vatamadhiṭṭhāsiṁ,",
  "bv21:23.4_0": "dasapāramipūriyā.",
  "bv21:24.1_0": "Nagaraṁ bandhumatī nāma,",
  "bv21:24.2_0": "bandhumā nāma khattiyo;",
  "bv21:24.3_0": "Mātā bandhumatī nāma,",
  "bv21:24.4_0": "vipassissa mahesino.",
  "bv21:25.1_0": "Aṭṭhavassasahassāni,",
  "bv21:25.2_0": "agāraṁ ajjha so vasi;",
  "bv21:25.3_0": "Nando sunando sirimā,",
  "bv21:25.4_0": "tayo pāsādamuttamā.",
  "bv21:26.1_0": "Ticattārīsasahassāni,",
  "bv21:26.2_0": "Nāriyo samalaṅkatā;",
  "bv21:26.3_0": "Sudassanā nāma sā nārī,",
  "bv21:26.4_0": "Samavattakkhandho nāma atrajo.",
  "bv21:27.1_0": "Nimitte caturo disvā,",
  "bv21:27.2_0": "rathayānena nikkhami;",
  "bv21:27.3_0": "Anūnaaṭṭhamāsāni,",
  "bv21:27.4_0": "padhānaṁ padahī jino.",
  "bv21:28.1_0": "Brahmunā yācito santo,",
  "bv21:28.2_0": "vipassī lokanāyako;",
  "bv21:28.3_0": "Vatti cakkaṁ mahāvīro,",
  "bv21:28.4_0": "migadāye naruttamo.",
  "bv21:29.1_0": "Khaṇḍo ca tissanāmo ca,",
  "bv21:29.2_0": "ahesuṁ aggasāvakā;",
  "bv21:29.3_0": "Asoko nāmupaṭṭhāko,",
  "bv21:29.4_0": "vipassissa mahesino.",
  "bv21:30.1_0": "Candā ca candamittā ca,",
  "bv21:30.2_0": "ahesuṁ aggasāvikā;",
  "bv21:30.3_0": "Bodhi tassa bhagavato,",
  "bv21:30.4_0": "pāṭalīti pavuccati.",
  "bv21:31.1_0": "Punabbasumitto nāgo ca,",
  "bv21:31.2_0": "ahesuṁ aggupaṭṭhakā;",
  "bv21:31.3_0": "Sirimā uttarā ceva,",
  "bv21:31.4_0": "ahesuṁ aggupaṭṭhikā.",
  "bv21:32.1_0": "Asītihatthamubbedho,",
  "bv21:32.2_0": "vipassī lokanāyako;",
  "bv21:32.3_0": "Pabhā niddhāvati tassa,",
  "bv21:32.4_0": "samantā sattayojane.",
  "bv21:33.1_0": "Asītivassasahassāni,",
  "bv21:33.2_0": "āyu buddhassa tāvade;",
  "bv21:33.3_0": "Tāvatā tiṭṭhamāno so,",
  "bv21:33.4_0": "tāresi janataṁ bahuṁ.",
  "bv21:34.1_0": "Bahudevamanussānaṁ,",
  "bv21:34.2_0": "bandhanā parimocayi;",
  "bv21:34.3_0": "Maggāmaggañca ācikkhi,",
  "bv21:34.4_0": "avasesaputhujjane.",
  "bv21:35.1_0": "Ālokaṁ dassayitvāna,",
  "bv21:35.2_0": "desetvā amataṁ padaṁ;",
  "bv21:35.3_0": "Jalitvā aggikkhandhova,",
  "bv21:35.4_0": "nibbuto so sasāvako.",
  "bv21:36.1_0": "Iddhivaraṁ puññavaraṁ,",
  "bv21:36.2_0": "lakkhaṇañca kusumitaṁ;",
  "bv21:36.3_0": "Sabbaṁ tamantarahitaṁ,",
  "bv21:36.4_0": "nanu rittā sabbasaṅkhārā.",
  "bv21:37.1_0": "Vipassī jinavaro buddho,",
  "bv21:37.2_0": "Sumittārāmamhi nibbuto;",
  "bv21:37.3_0": "Tatthevassa thūpavaro,",
  "bv21:37.4_0": "Sattayojanamussito”ti. ",
  "bv21:38.1_0": "Vipassissa bhagavato vaṁso ekūnavīsatimo."
}