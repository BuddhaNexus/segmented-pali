{
  "tha-ap354:0.1_0": "Therāpadāna",
  "tha-ap354:0.2_0": "Saddasaññakavagga",
  "tha-ap354:0.3_0": "2. Yavakalāpiyattheraapadāna",
  "tha-ap354:1.1_0": "“Nagare aruṇavatiyā,",
  "tha-ap354:1.2_0": "āsiṁ yavasiko tadā;",
  "tha-ap354:1.3_0": "Panthe disvāna sambuddhaṁ,",
  "tha-ap354:1.4_0": "yavakalāpaṁ santhariṁ.",
  "tha-ap354:2.1_0": "Anukampako kāruṇiko,",
  "tha-ap354:2.2_0": "sikhī lokagganāyako;",
  "tha-ap354:2.3_0": "Mama saṅkappamaññāya,",
  "tha-ap354:2.4_0": "nisīdi yavasanthare.",
  "tha-ap354:3.1_0": "Disvā nisinnaṁ vimalaṁ,",
  "tha-ap354:3.2_0": "mahājhāyiṁ vināyakaṁ;",
  "tha-ap354:3.3_0": "Pāmojjaṁ janayitvāna,",
  "tha-ap354:3.4_0": "tattha kālaṅkato ahaṁ.",
  "tha-ap354:4.1_0": "Ekattiṁse ito kappe,",
  "tha-ap354:4.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap354:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap354:4.4_0": "yavatthare idaṁ phalaṁ.",
  "tha-ap354:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap354:5.2_0": "…pe…",
  "tha-ap354:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap354:6.1_0": "Itthaṁ sudaṁ āyasmā yavakalāpiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap354:7.1_0": "Yavakalāpiyattherassāpadānaṁ dutiyaṁ."
}