{
  "tha-ap467:0.1_0": "Therāpadāna",
  "tha-ap467:0.2_0": "Sālakusumiyavagga",
  "tha-ap467:0.3_0": "5. Sumanatālavaṇṭiyattheraapadāna",
  "tha-ap467:1.1_0": "“Siddhatthassa bhagavato,",
  "tha-ap467:1.2_0": "tālavaṇṭamadāsahaṁ;",
  "tha-ap467:1.3_0": "Sumanehi paṭicchannaṁ,",
  "tha-ap467:1.4_0": "dhārayāmi mahāyasaṁ.",
  "tha-ap467:2.1_0": "Catunnavutito kappe,",
  "tha-ap467:2.2_0": "tālavaṇṭamadāsahaṁ;",
  "tha-ap467:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap467:2.4_0": "tālavaṇṭassidaṁ phalaṁ.",
  "tha-ap467:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap467:3.2_0": "…pe…",
  "tha-ap467:3.3_0": "viharāmi anāsavo.",
  "tha-ap467:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap467:4.2_0": "…pe…",
  "tha-ap467:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap467:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap467:5.2_0": "…pe…",
  "tha-ap467:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap467:6.1_0": "Itthaṁ sudaṁ āyasmā sumanatālavaṇṭiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap467:7.1_0": "Sumanatālavaṇṭiyattherassāpadānaṁ pañcamaṁ."
}