{
  "thag1.39:0.1_0": "Theragāthā",
  "thag1.39:0.2_0": "Ekakanipāta",
  "thag1.39:0.3_0": "Catutthavagga",
  "thag1.39:0.4_0": "1.39. Tissattheragāthā",
  "thag1.39:1.1_0": "“Sattiyā viya omaṭṭho,",
  "thag1.39:1.2_0": "ḍayhamānova matthake;",
  "thag1.39:1.3_0": "Kāmarāgappahānāya,",
  "thag1.39:1.4_0": "sato bhikkhu paribbaje”ti.",
  "thag1.39:1.5_0": "… Tisso thero …."
}