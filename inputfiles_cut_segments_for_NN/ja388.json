{
  "ja388:0.1_0": "Jātaka",
  "ja388:0.2_0": "Chakkanipāta",
  "ja388:0.3_0": "Kharaputtavagga",
  "ja388:0.4_0": "3. Tuṇḍilajātaka",
  "ja388:1.1_0": "“Navachannake dāni diyyati,",
  "ja388:1.2_0": "Puṇṇāyaṁ doṇi suvāminī ṭhitā;",
  "ja388:1.3_0": "Bahuke jane pāsapāṇike,",
  "ja388:1.4_0": "No ca kho me paṭibhāti bhuñjituṁ”.",
  "ja388:2.1_0": "“Tasasi bhamasi leṇamicchasi,",
  "ja388:2.2_0": "Attāṇosi kuhiṁ gamissasi;",
  "ja388:2.3_0": "Appossukko bhuñja tuṇḍila,",
  "ja388:2.4_0": "Maṁsatthāya hi positāmhase.",
  "ja388:3.1_0": "Ogaha rahadaṁ akaddamaṁ,",
  "ja388:3.2_0": "Sabbaṁ sedamalaṁ pavāhaya;",
  "ja388:3.3_0": "Gaṇhāhi navaṁ vilepanaṁ,",
  "ja388:3.4_0": "Yassa gandho na kadāci chijjati”.",
  "ja388:4.1_0": "“Katamo rahado akaddamo,",
  "ja388:4.2_0": "Kiṁsu sedamalanti vuccati;",
  "ja388:4.3_0": "Katamañca navaṁ vilepanaṁ,",
  "ja388:4.4_0": "Yassa gandho na kadāci chijjati”.",
  "ja388:5.1_0": "“Dhammo rahado akaddamo,",
  "ja388:5.2_0": "Pāpaṁ sedamalanti vuccati;",
  "ja388:5.3_0": "Sīlañca navaṁ vilepanaṁ,",
  "ja388:5.4_0": "Tassa gandho na kadāci chijjati.",
  "ja388:6.1_0": "Nandanti sarīraghātino,",
  "ja388:6.2_0": "Na ca nandanti sarīradhārino;",
  "ja388:6.3_0": "Puṇṇāya ca puṇṇamāsiyā,",
  "ja388:6.4_0": "Ramamānāva jahanti jīvitan”ti.",
  "ja388:7.1_0": "Tuṇḍilajātakaṁ tatiyaṁ."
}