{
  "patthana16.30:0.1_0": "Paṭṭhānapakaraṇa",
  "patthana16.30:0.2_0": "Dhammānulomapaccanīya (3), Tikadukapaṭṭhānapāḷi (4)",
  "patthana16.30:0.3_0": "16.30. Vitakkattika, Hetuduka",
  "patthana16.30:1.1_0": "Savitakkasavicāraṁ hetuṁ dhammaṁ paṭicca nasavitakkasavicāro nahetu dhammo uppajjati hetupaccayā.",
  "patthana16.30:1.1_1": "(Saṅkhittaṁ.) Hetuyā pannarasa …pe…",
  "patthana16.30:1.1_2": "avigate pannarasa.",
  "patthana16.30:2.1_0": "Savitakkasavicāraṁ nahetuṁ dhammaṁ paṭicca naavitakkavicāramatto nanahetu dhammo uppajjati hetupaccayā.",
  "patthana16.30:2.1_1": "(Saṅkhittaṁ.) Hetuyā aṭṭhavīsa,",
  "patthana16.30:2.1_2": "ārammaṇe aṭṭhavīsa,",
  "patthana16.30:2.1_3": "adhipatiyā ekūnavīsa …pe…",
  "patthana16.30:2.1_4": "purejāte āsevane ekūnavīsa …pe…",
  "patthana16.30:2.1_5": "avigate aṭṭhavīsa."
}