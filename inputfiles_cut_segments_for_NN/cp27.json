{
  "cp27:0.1_0": "Cariyāpiṭaka",
  "cp27:0.2_0": "Yudhañjayavagga",
  "cp27:0.3_0": "Saccapāramī 1",
  "cp27:0.4_0": "Kapirājacariya",
  "cp27:1.1_0": "“Yadā ahaṁ kapi āsiṁ,",
  "cp27:1.2_0": "nadīkūle darīsaye;",
  "cp27:1.3_0": "Pīḷito susumārena,",
  "cp27:1.4_0": "gamanaṁ na labhāmahaṁ.",
  "cp27:2.1_0": "Yamhokāse ahaṁ ṭhatvā,",
  "cp27:2.2_0": "orā pāraṁ patāmahaṁ;",
  "cp27:2.3_0": "Tatthacchi sattu vadhako,",
  "cp27:2.4_0": "kumbhīlo luddadassano.",
  "cp27:3.1_0": "So maṁ asaṁsi ‘ehī’ti,",
  "cp27:3.2_0": "‘ahampemī’ti taṁ vatiṁ;",
  "cp27:3.3_0": "Tassa matthakamakkamma,",
  "cp27:3.4_0": "parakūle patiṭṭhahiṁ.",
  "cp27:4.1_0": "Na tassa alikaṁ bhaṇitaṁ,",
  "cp27:4.2_0": "yathā vācaṁ akāsahaṁ;",
  "cp27:4.3_0": "Saccena me samo natthi,",
  "cp27:4.4_0": "esā me saccapāramī”ti.",
  "cp27:4.5_0": "Kapirājacariyaṁ sattamaṁ."
}