{
  "thag1.84:0.1_0": "Theragāthā",
  "thag1.84:0.2_0": "Ekakanipāta",
  "thag1.84:0.3_0": "Navamavagga",
  "thag1.84:0.4_0": "1.84. Nītattheragāthā",
  "thag1.84:1.1_0": "“Sabbarattiṁ supitvāna,",
  "thag1.84:1.2_0": "divā saṅgaṇike rato;",
  "thag1.84:1.3_0": "Kudāssu nāma dummedho,",
  "thag1.84:1.4_0": "dukkhassantaṁ karissatī”ti.",
  "thag1.84:1.5_0": "… Nīto thero …."
}