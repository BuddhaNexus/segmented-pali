{
  "tha-ap170:0.1_0": "Therāpadāna",
  "tha-ap170:0.2_0": "Supāricariyavagga",
  "tha-ap170:0.3_0": "8. Dussadāyakattheraapadāna",
  "tha-ap170:1.1_0": "“Tivarāyaṁ pure ramme,",
  "tha-ap170:1.2_0": "rājaputtosahaṁ tadā;",
  "tha-ap170:1.3_0": "Paṇṇākāraṁ labhitvāna,",
  "tha-ap170:1.4_0": "upasantassadāsahaṁ.",
  "tha-ap170:2.1_0": "Adhivāsesi bhagavā,",
  "tha-ap170:2.2_0": "vatthaṁ hatthena āmasi;",
  "tha-ap170:2.3_0": "Siddhattho adhivāsetvā,",
  "tha-ap170:2.4_0": "vehāsaṁ nabhamuggami.",
  "tha-ap170:3.1_0": "Buddhassa gacchamānassa,",
  "tha-ap170:3.2_0": "dussā dhāvanti pacchato;",
  "tha-ap170:3.3_0": "Tattha cittaṁ pasādesiṁ,",
  "tha-ap170:3.4_0": "buddho no aggapuggalo.",
  "tha-ap170:4.1_0": "Catunnavutito kappe,",
  "tha-ap170:4.2_0": "yaṁ dussamadadiṁ tadā;",
  "tha-ap170:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap170:4.4_0": "dussadānassidaṁ phalaṁ.",
  "tha-ap170:5.1_0": "Sattasaṭṭhimhito kappe,",
  "tha-ap170:5.2_0": "cakkavattī tadā ahuṁ;",
  "tha-ap170:5.3_0": "Parisuddhoti nāmena,",
  "tha-ap170:5.4_0": "manujindo mahabbalo.",
  "tha-ap170:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap170:6.2_0": "…pe…",
  "tha-ap170:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap170:7.1_0": "Itthaṁ sudaṁ āyasmā dussadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap170:8.1_0": "Dussadāyakattherassāpadānaṁ aṭṭhamaṁ."
}