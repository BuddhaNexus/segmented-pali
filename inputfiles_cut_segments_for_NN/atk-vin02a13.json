{
  "atk-vin02a13:0_0": "8. Sattādhikaraṇasamathā",
  "atk-vin02a13:1_0": "655.",
  "atk-vin02a13:1_1": "Adhikaraṇasamathesu –",
  "atk-vin02a13:1_2": "sattāti tesaṁ dhammānaṁ saṅkhyāparicchedo.",
  "atk-vin02a13:1_3": "Catubbidhaṁ adhikaraṇaṁ samenti vūpasamentīti adhikaraṇasamathā.",
  "atk-vin02a13:1_4": "Tesaṁ vitthāro khandhake ca parivāre ca vutto,",
  "atk-vin02a13:1_5": "tassatthaṁ tattheva vaṇṇayissāma.",
  "atk-vin02a13:1_6": "Sesaṁ sabbattha uttānamevāti.",
  "atk-vin02a13:2_0": "Samantapāsādikāya vinayasaṁvaṇṇanāya",
  "atk-vin02a13:3_0": "Bhikkhuvibhaṅgavaṇṇanā niṭṭhitā.",
  "atk-vin02a13:4_0": "Anantarāyena yathā, niṭṭhitā vaṇṇanā ayaṁ;",
  "atk-vin02a13:5_0": "Anantarāyena tathā, santiṁ pappontu pāṇino.",
  "atk-vin02a13:6_0": "Ciraṁ tiṭṭhatu saddhammo, kāle vassaṁ ciraṁ pajaṁ;",
  "atk-vin02a13:7_0": "Tappetu devo dhammena, rājā rakkhatu medaninti.",
  "atk-vin02a13:8_0": "Mahāvibhaṅgo niṭṭhito.",
  "atk-vin02a13:9_0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "atk-vin02a13:10_0": "Bhikkhunīvibhaṅgavaṇṇanā"
}