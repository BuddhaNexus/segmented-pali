{
  "mil3.7.14:0.1_0": "Milindapañha",
  "mil3.7.14:0.2_0": "Arūpadhammavavatthānavagga",
  "mil3.7.14:0.3_0": "14. Sukhumapañha",
  "mil3.7.14:1.1_0": "Rājā āha—",
  "mil3.7.14:1.2_0": "“bhante nāgasena,",
  "mil3.7.14:1.2_1": "sakkā sabbaṁ sukhumaṁ chinditun”ti?",
  "mil3.7.14:1.3_0": "“Āma, mahārāja,",
  "mil3.7.14:1.3_1": "sakkā sabbaṁ sukhumaṁ chinditun”ti.",
  "mil3.7.14:1.4_0": "“Kiṁ pana, bhante, sabbaṁ sukhuman”ti?",
  "mil3.7.14:1.5_0": "“Dhammo kho, mahārāja, sabbasukhumo, na kho, mahārāja, dhammā sabbe sukhumā,",
  "mil3.7.14:1.5_1": "‘sukhuman’ti vā ‘thūlan’ti vā dhammānametamadhivacanaṁ.",
  "mil3.7.14:1.6_0": "Yaṁ kiñci chinditabbaṁ, sabbaṁ taṁ paññāya chindati,",
  "mil3.7.14:1.6_1": "natthi dutiyaṁ paññāya chedanan”ti.",
  "mil3.7.14:2.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.7.14:3.1_0": "Sukhumapañho cuddasamo."
}