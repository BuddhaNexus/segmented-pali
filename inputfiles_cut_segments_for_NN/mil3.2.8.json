{
  "mil3.2.8:0.1_0": "Milindapañha",
  "mil3.2.8:0.2_0": "Addhānavagga",
  "mil3.2.8:0.3_0": "8. Nāmarūpapaṭisandahanapañha",
  "mil3.2.8:1.1_0": "Rājā āha—",
  "mil3.2.8:1.2_0": "“bhante nāgasena, yaṁ panetaṁ brūsi ‘nāmarūpan’ti, tattha katamaṁ nāmaṁ, katamaṁ rūpan”ti.",
  "mil3.2.8:1.3_0": "“Yaṁ tattha, mahārāja, oḷārikaṁ, etaṁ rūpaṁ, ye tattha sukhumā cittacetasikā dhammā, etaṁ nāman”ti.",
  "mil3.2.8:1.4_0": "“Bhante nāgasena, kena kāraṇena nāmaṁyeva na paṭisandahati, rūpaṁyeva vā”ti?",
  "mil3.2.8:1.5_0": "“Aññamaññūpanissitā, mahārāja, ete dhammā ekatova uppajjantī”ti.",
  "mil3.2.8:2.1_0": "“Opammaṁ karohī”ti.",
  "mil3.2.8:2.2_0": "“Yathā, mahārāja,",
  "mil3.2.8:2.2_1": "kukkuṭiyā kalalaṁ na bhaveyya,",
  "mil3.2.8:2.2_2": "aṇḍampi na bhaveyya,",
  "mil3.2.8:2.2_3": "yañca tattha kalalaṁ,",
  "mil3.2.8:2.2_4": "yañca aṇḍaṁ,",
  "mil3.2.8:2.2_5": "ubhopete aññamaññūpanissitā,",
  "mil3.2.8:2.2_6": "ekatova nesaṁ uppatti hoti;",
  "mil3.2.8:2.3_0": "evameva kho, mahārāja,",
  "mil3.2.8:2.3_1": "yadi tattha nāmaṁ na bhaveyya,",
  "mil3.2.8:2.3_2": "rūpampi na bhaveyya,",
  "mil3.2.8:2.3_3": "yañceva tattha nāmaṁ,",
  "mil3.2.8:2.3_4": "yañceva rūpaṁ,",
  "mil3.2.8:2.3_5": "ubhopete aññamaññūpanissitā,",
  "mil3.2.8:2.3_6": "ekatova nesaṁ uppatti hoti.",
  "mil3.2.8:2.4_0": "Evametaṁ dīghamaddhānaṁ sandhāvitan”ti.",
  "mil3.2.8:3.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.2.8:4.1_0": "Nāmarūpapaṭisandahanapañho aṭṭhamo."
}