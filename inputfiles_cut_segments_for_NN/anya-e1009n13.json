{
  "anya-e1009n13:0_0": "Naradakkha thomanā āsīsa",
  "anya-e1009n13:1_0": "3. Gāthā",
  "anya-e1009n13:2_0": "1.",
  "anya-e1009n13:3_0": " Pālito pāḷiyā cheko,",
  "anya-e1009n13:4_0": "Tvaṁsi gambhīra-ñāṇavā;",
  "anya-e1009n13:5_0": "Pāliyāva pālitassa,",
  "anya-e1009n13:6_0": "Dadāmidāni bho ahaṁ.",
  "anya-e1009n13:7_0": "2.",
  "anya-e1009n13:8_0": " Dakkhāvādesu kusalo,",
  "anya-e1009n13:9_0": "Pālito sāsanandharī;",
  "anya-e1009n13:10_0": "Piṭakesu ajjhogāyha,",
  "anya-e1009n13:11_0": "Naradakkhaṁbhisaṅkharī.",
  "anya-e1009n13:12_0": "3.",
  "anya-e1009n13:13_0": " Suta-dharena racitaṁ,",
  "anya-e1009n13:14_0": "Etaṁ sāra-gavesino;",
  "anya-e1009n13:15_0": "Atandikā su-dakkhantu,",
  "anya-e1009n13:16_0": "Aggagga-sāsane ratā.",
  "anya-e1009n13:17_0": "Naradakkha thomanā āsīsa",
  "anya-e1009n13:18_0": "2. Gāthā",
  "anya-e1009n13:19_0": "1.",
  "anya-e1009n13:20_0": " Ñuṁ pālitodha jāto yo,",
  "anya-e1009n13:21_0": "Thero so abbhutova ñuṁ;",
  "anya-e1009n13:22_0": "Ñuṁ mahā-pālito santo,",
  "anya-e1009n13:23_0": "Nikāya-pālito ca ñuṁ.",
  "anya-e1009n13:24_0": "2.",
  "anya-e1009n13:25_0": " Ñuṁ nara-dakkha-ganthaṁ yaṁ,",
  "anya-e1009n13:26_0": "Sovakā nara-dakkha-daṁ;",
  "anya-e1009n13:27_0": "Narā dakkhantu sammā ca,",
  "anya-e1009n13:28_0": "Dakkhattaṁ pāpuṇantu ñuṁ."
}