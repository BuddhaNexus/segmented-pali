{
  "tha-ap233:0.1_0": "Therāpadāna",
  "tha-ap233:0.2_0": "Udakāsanavagga",
  "tha-ap233:0.3_0": "1. Udakāsanadāyakattheraapadāna",
  "tha-ap233:1.1_0": "“Ārāmadvārā nikkhamma,",
  "tha-ap233:1.2_0": "phalakaṁ santhariṁ ahaṁ;",
  "tha-ap233:1.3_0": "Udakañca upaṭṭhāsiṁ,",
  "tha-ap233:1.4_0": "uttamatthassa pattiyā.",
  "tha-ap233:2.1_0": "Ekattiṁse ito kappe,",
  "tha-ap233:2.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap233:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap233:2.4_0": "āsane codake phalaṁ.",
  "tha-ap233:3.1_0": "Ito pannarase kappe,",
  "tha-ap233:3.2_0": "abhisāmasamavhayo;",
  "tha-ap233:3.3_0": "Sattaratanasampanno,",
  "tha-ap233:3.4_0": "cakkavattī mahabbalo.",
  "tha-ap233:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap233:4.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap233:4.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap233:4.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap233:5.1_0": "Itthaṁ sudaṁ āyasmā udakāsanadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap233:6.1_0": "Udakāsanadāyakattherassāpadānaṁ paṭhamaṁ."
}