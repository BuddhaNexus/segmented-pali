{
  "kv4.1:0.1_0": "Kathāvatthu",
  "kv4.1:0.2_0": "Mahāpaṇṇāsaka",
  "kv4.1:0.3_0": "Catutthavagga",
  "kv4.1:0.4_0": "Gihissaarahātikathā",
  "kv4.1:1.1_0": "Gihi’ssa arahāti?",
  "kv4.1:1.2_0": "Āmantā.",
  "kv4.1:1.3_0": "Atthi arahato gihisaṁyojananti?",
  "kv4.1:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv4.1:1.5_0": "natthi arahato gihisaṁyojananti?",
  "kv4.1:1.6_0": "Āmantā.",
  "kv4.1:1.7_0": "Hañci natthi arahato gihisaṁyojanaṁ, no ca vata re vattabbe—",
  "kv4.1:1.8_0": "“gihi’ssa arahā”ti.",
  "kv4.1:2.1_0": "Gihi’ssa arahāti?",
  "kv4.1:2.2_0": "Āmantā.",
  "kv4.1:2.3_0": "Nanu arahato gihisaṁyojanaṁ pahīnaṁ ucchinnamūlaṁ tālāvatthukataṁ anabhāvaṅkataṁ āyatiṁ anuppādadhammanti?",
  "kv4.1:2.4_0": "Āmantā.",
  "kv4.1:2.5_0": "Hañci arahato gihisaṁyojanaṁ pahīnaṁ ucchinnamūlaṁ tālāvatthukataṁ anabhāvaṅkataṁ āyatiṁ anuppādadhammaṁ,",
  "kv4.1:2.5_1": "no ca vata re vattabbe—",
  "kv4.1:2.6_0": "“gihi’ssa arahā”ti.",
  "kv4.1:3.1_0": "Gihi’ssa arahāti?",
  "kv4.1:3.2_0": "Āmantā.",
  "kv4.1:3.3_0": "Atthi koci gihī gihisaṁyojanaṁ appahāya diṭṭheva dhamme dukkhassantakaroti?",
  "kv4.1:3.4_0": "Natthi.",
  "kv4.1:3.5_0": "Hañci natthi koci gihī gihisaṁyojanaṁ appahāya diṭṭheva dhamme dukkhassantakaro,",
  "kv4.1:3.5_1": "no ca vata re vattabbe—",
  "kv4.1:3.6_0": "“gihi’ssa arahā”ti.",
  "kv4.1:4.1_0": "Gihi’ssa arahāti?",
  "kv4.1:4.2_0": "Āmantā.",
  "kv4.1:4.3_0": "Nanu vacchagotto paribbājako bhagavantaṁ etadavoca—",
  "kv4.1:4.4_0": "“atthi nu kho, bho gotama, koci gihī gihisaṁyojanaṁ appahāya kāyassa bhedā dukkhassantakaro”ti?",
  "kv4.1:4.5_0": "“Natthi kho, vaccha, koci gihī gihisaṁyojanaṁ appahāya kāyassa bhedā dukkhassantakaro”ti.",
  "kv4.1:4.6_0": "Attheva suttantoti?",
  "kv4.1:4.7_0": "Āmantā.",
  "kv4.1:4.8_0": "Tena hi na vattabbaṁ—",
  "kv4.1:4.9_0": "“gihi’ssa arahā”ti.",
  "kv4.1:5.1_0": "Gihi’ssa arahāti?",
  "kv4.1:5.2_0": "Āmantā.",
  "kv4.1:5.3_0": "Arahā methunaṁ dhammaṁ paṭiseveyya,",
  "kv4.1:5.3_1": "methunaṁ uppādeyya,",
  "kv4.1:5.3_2": "puttasambādhasayanaṁ ajjhāvaseyya,",
  "kv4.1:5.3_3": "kāsikacandanaṁ paccanubhaveyya,",
  "kv4.1:5.3_4": "mālāgandhavilepanaṁ dhāreyya,",
  "kv4.1:5.3_5": "jātarūparajataṁ sādiyeyya,",
  "kv4.1:5.3_6": "ajeḷakaṁ paṭiggaṇheyya,",
  "kv4.1:5.3_7": "kukkuṭasūkaraṁ paṭiggaṇheyya,",
  "kv4.1:5.3_8": "hatthigavassavaḷavaṁ paṭiggaṇheyya,",
  "kv4.1:5.3_9": "tittiravaṭṭakamorakapiñjaraṁ paṭiggaṇheyya,",
  "kv4.1:5.3_10": "cittavaṇḍavālamoḷiṁ dhāreyya,",
  "kv4.1:5.3_11": "odātāni vatthāni dīghadasāni dhāreyya,",
  "kv4.1:5.3_12": "yāvajīvaṁ agāriyabhūto assāti?",
  "kv4.1:5.4_0": "Na hevaṁ vattabbe …pe….",
  "kv4.1:6.1_0": "Na vattabbaṁ—",
  "kv4.1:6.2_0": "“gihi’ssa arahā”ti?",
  "kv4.1:6.3_0": "Āmantā.",
  "kv4.1:6.4_0": "Nanu yaso kulaputto, uttiyo gahapati, setu māṇavo, gihibyañjanena arahattaṁ pattoti?",
  "kv4.1:6.5_0": "Āmantā.",
  "kv4.1:6.6_0": "Hañci yaso kulaputto,",
  "kv4.1:6.6_1": "uttiyo gahapati,",
  "kv4.1:6.6_2": "setu māṇavo,",
  "kv4.1:6.6_3": "gihibyañjanena arahattaṁ patto,",
  "kv4.1:6.6_4": "tena vata re vattabbe—",
  "kv4.1:6.7_0": "“gihi’ssa arahā”ti.",
  "kv4.1:6.8_0": "Gihi’ssa arahātikathā niṭṭhitā."
}