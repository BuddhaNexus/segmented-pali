{
  "pli-tv-bi-vb-pc83:0.1_0": "Theravāda Vinaya",
  "pli-tv-bi-vb-pc83:0.2_0": "Bhikkhunivibhaṅga",
  "pli-tv-bi-vb-pc83:0.3_0": "Pācittiyakaṇḍa",
  "pli-tv-bi-vb-pc83:0.4_0": "Kumāribhūtavagga",
  "pli-tv-bi-vb-pc83:0.5_0": "83. Ekavassasikkhāpada",
  "pli-tv-bi-vb-pc83:1.1_0": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bi-vb-pc83:1.2_0": "Tena kho pana samayena bhikkhuniyo ekaṁ vassaṁ dve vuṭṭhāpenti.",
  "pli-tv-bi-vb-pc83:1.3_0": "Upassayo tatheva na sammati.",
  "pli-tv-bi-vb-pc83:1.4_0": "Manussā tatheva ujjhāyanti khiyyanti vipācenti—",
  "pli-tv-bi-vb-pc83:1.5_0": "“kathañhi nāma bhikkhuniyo ekaṁ vassaṁ dve vuṭṭhāpessanti.",
  "pli-tv-bi-vb-pc83:1.6_0": "Upassayo tatheva na sammatī”ti.",
  "pli-tv-bi-vb-pc83:1.7_0": "Assosuṁ kho bhikkhuniyo tesaṁ manussānaṁ ujjhāyantānaṁ khiyyantānaṁ vipācentānaṁ.",
  "pli-tv-bi-vb-pc83:1.8_0": "Yā tā bhikkhuniyo appicchā …pe… tā ujjhāyanti khiyyanti vipācenti—",
  "pli-tv-bi-vb-pc83:1.9_0": "“kathañhi nāma bhikkhuniyo ekaṁ vassaṁ dve vuṭṭhāpessantī”ti …pe…",
  "pli-tv-bi-vb-pc83:1.10_0": "“saccaṁ kira, bhikkhave,",
  "pli-tv-bi-vb-pc83:1.10_1": "bhikkhuniyo ekaṁ vassaṁ dve vuṭṭhāpentī”ti?",
  "pli-tv-bi-vb-pc83:1.11_0": "“Saccaṁ, bhagavā”ti.",
  "pli-tv-bi-vb-pc83:1.12_0": "Vigarahi buddho bhagavā …pe…",
  "pli-tv-bi-vb-pc83:1.13_0": "kathañhi nāma, bhikkhave,",
  "pli-tv-bi-vb-pc83:1.13_1": "bhikkhuniyo ekaṁ vassaṁ dve vuṭṭhāpessanti.",
  "pli-tv-bi-vb-pc83:1.14_0": "Netaṁ, bhikkhave, appasannānaṁ vā pasādāya …pe…",
  "pli-tv-bi-vb-pc83:1.15_0": "evañca pana, bhikkhave,",
  "pli-tv-bi-vb-pc83:1.15_1": "bhikkhuniyo imaṁ sikkhāpadaṁ uddisantu—",
  "pli-tv-bi-vb-pc83:1.16.1_0": "“Yā pana bhikkhunī ekaṁ vassaṁ dve vuṭṭhāpeyya, pācittiyan”ti.",
  "pli-tv-bi-vb-pc83:2.1.1_0": "Yā panāti",
  "pli-tv-bi-vb-pc83:2.1.2_0": "yā yādisā …pe…",
  "pli-tv-bi-vb-pc83:2.1.3_0": "bhikkhunīti",
  "pli-tv-bi-vb-pc83:2.1.4_0": "…pe… ayaṁ imasmiṁ atthe adhippetā bhikkhunīti.",
  "pli-tv-bi-vb-pc83:2.1.5_0": "Ekaṁ vassanti",
  "pli-tv-bi-vb-pc83:2.1.6_0": "ekaṁ saṁvaccharaṁ.",
  "pli-tv-bi-vb-pc83:2.1.7_0": "Dve vuṭṭhāpeyyāti",
  "pli-tv-bi-vb-pc83:2.1.8_0": "dve upasampādeyya.",
  "pli-tv-bi-vb-pc83:2.1.9_0": "“Dve vuṭṭhāpessāmī”ti gaṇaṁ vā ācariniṁ vā pattaṁ vā cīvaraṁ vā pariyesati, sīmaṁ vā sammannati, āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc83:2.1.10_0": "Ñattiyā dukkaṭaṁ.",
  "pli-tv-bi-vb-pc83:2.1.11_0": "Dvīhi kammavācāhi dukkaṭā.",
  "pli-tv-bi-vb-pc83:2.1.12_0": "Kammavācāpariyosāne upajjhāyāya āpatti pācittiyassa.",
  "pli-tv-bi-vb-pc83:2.1.13_0": "Gaṇassa ca ācariniyā ca āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc83:2.2.1_0": "Anāpatti—",
  "pli-tv-bi-vb-pc83:2.2.2_0": "ekantarikaṁ ekaṁ vuṭṭhāpeti,",
  "pli-tv-bi-vb-pc83:2.2.3_0": "ummattikāya,",
  "pli-tv-bi-vb-pc83:2.2.4_0": "ādikammikāyāti.",
  "pli-tv-bi-vb-pc83:2.2.5_0": "Terasamasikkhāpadaṁ niṭṭhitaṁ.",
  "pli-tv-bi-vb-pc83:2.2.6_0": "Kumāribhūtavaggo aṭṭhamo."
}