{
  "kv21.8:0.1_0": "Kathāvatthu",
  "kv21.8:0.2_0": "Mahāpaṇṇāsaka",
  "kv21.8:0.3_0": "Ekavīsatimavagga",
  "kv21.8:0.4_0": "Kammakathā",
  "kv21.8:1.1_0": "Sabbe kammā niyatāti?",
  "kv21.8:1.2_0": "Āmantā.",
  "kv21.8:1.3_0": "Micchattaniyatāti?",
  "kv21.8:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv21.8:1.5_0": "sammattaniyatāti?",
  "kv21.8:1.6_0": "Na hevaṁ vattabbe …pe…",
  "kv21.8:1.7_0": "natthi aniyato rāsīti?",
  "kv21.8:1.8_0": "Na hevaṁ vattabbe …pe…",
  "kv21.8:1.9_0": "nanu atthi aniyato rāsīti?",
  "kv21.8:1.10_0": "Āmantā.",
  "kv21.8:1.11_0": "Hañci atthi aniyato rāsi, no ca vata re vattabbe—",
  "kv21.8:1.12_0": "“sabbe kammā niyatā”ti.",
  "kv21.8:2.1_0": "Sabbe kammā niyatāti?",
  "kv21.8:2.2_0": "Āmantā.",
  "kv21.8:2.3_0": "Nanu tayo rāsī vuttā bhagavatā—",
  "kv21.8:2.4_0": "“micchattaniyato rāsi, sammattaniyato rāsi, aniyato rāsī”ti?",
  "kv21.8:2.5_0": "Āmantā.",
  "kv21.8:2.6_0": "Hañci tayo rāsī vuttā bhagavatā—",
  "kv21.8:2.7_0": "“micchattaniyato rāsi, sammattaniyato rāsi, aniyato rāsi”, no ca vata re vattabbe—",
  "kv21.8:2.8_0": "“sabbe kammā niyatā”ti.",
  "kv21.8:3.1_0": "Diṭṭhadhammavedanīyaṁ kammaṁ diṭṭhadhammavedanīyaṭṭhena niyatanti?",
  "kv21.8:3.2_0": "Āmantā.",
  "kv21.8:3.3_0": "Micchattaniyatanti?",
  "kv21.8:3.4_0": "Na hevaṁ vattabbe …pe…",
  "kv21.8:3.5_0": "sammattaniyatanti?",
  "kv21.8:3.6_0": "Na hevaṁ vattabbe …pe….",
  "kv21.8:3.7_0": "Upapajjavedanīyaṁ kammaṁ …pe…",
  "kv21.8:3.8_0": "aparāpariyavedanīyaṁ kammaṁ aparāpariyavedanīyaṭṭhena niyatanti?",
  "kv21.8:3.9_0": "Āmantā.",
  "kv21.8:3.10_0": "Micchattaniyatanti?",
  "kv21.8:3.11_0": "Na hevaṁ vattabbe …pe…",
  "kv21.8:3.12_0": "sammattaniyatanti?",
  "kv21.8:3.13_0": "Na hevaṁ vattabbe …pe….",
  "kv21.8:4.1_0": "Na vattabbaṁ—",
  "kv21.8:4.2_0": "diṭṭhadhammavedanīyaṁ kammaṁ diṭṭhadhammavedanīyaṭṭhena niyataṁ, upapajjavedanīyaṁ kammaṁ …pe…",
  "kv21.8:4.3_0": "aparāpariyavedanīyaṁ kammaṁ aparāpariyavedanīyaṭṭhena niyatanti?",
  "kv21.8:4.4_0": "Āmantā.",
  "kv21.8:4.5_0": "Diṭṭhadhammavedanīyaṁ kammaṁ upapajjavedanīyaṁ hoti,",
  "kv21.8:4.5_1": "aparāpariyavedanīyaṁ hoti …pe…",
  "kv21.8:4.6_0": "upapajjavedanīyaṁ kammaṁ diṭṭhadhammavedanīyaṁ hoti,",
  "kv21.8:4.6_1": "aparāpariyavedanīyaṁ hoti …pe…",
  "kv21.8:4.7_0": "aparāpariyavedanīyaṁ kammaṁ diṭṭhadhammavedanīyaṁ hoti, upapajjavedanīyaṁ hotīti?",
  "kv21.8:4.8_0": "Na hevaṁ vattabbe.",
  "kv21.8:4.9_0": "Tena hi diṭṭhadhammavedanīyaṁ kammaṁ diṭṭhadhammavedanīyaṭṭhena niyataṁ, upapajjavedanīyaṁ kammaṁ …pe…",
  "kv21.8:4.10_0": "aparāpariyavedanīyaṁ kammaṁ aparāpariyavedanīyaṭṭhena niyatanti.",
  "kv21.8:4.11_0": "Kammakathā niṭṭhitā.",
  "kv21.8:4.12_0": "Ekavīsatimo vaggo.",
  "kv21.8:4.13_0": "Tassuddānaṁ",
  "kv21.8:5.1_0": "Sāsanaṁ navaṁ kataṁ atthi koci tathāgatassa sāsanaṁ navaṁ karoti labbhā tathāgatassa sāsanaṁ puna navaṁ kātuṁ,",
  "kv21.8:5.1_1": "puthujjano tedhātukehi kammehi avivitto,",
  "kv21.8:5.1_2": "atthi kiñci saṁyojanaṁ appahāya arahattappatti,",
  "kv21.8:5.1_3": "atthi adhippāyiddhi buddhānaṁ vā sāvakānaṁ vā,",
  "kv21.8:5.1_4": "atthi buddhānaṁ buddhehi hīnātirekatā, sabbā disā buddhā tiṭṭhanti, sabbe dhammā niyatā, sabbe kammā niyatāti."
}