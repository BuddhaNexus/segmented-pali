{
  "tha-ap444:0.1_0": "Therāpadāna",
  "tha-ap444:0.2_0": "Vibhītakavagga",
  "tha-ap444:0.3_0": "2. Koladāyakattheraapadāna",
  "tha-ap444:1.1_0": "“Ajinena nivatthohaṁ,",
  "tha-ap444:1.2_0": "Vākacīradharo tadā;",
  "tha-ap444:1.3_0": "Khāriyā pūrayitvāna,",
  "tha-ap444:1.4_0": "Kolaṁhāsiṁ mamassamaṁ.",
  "tha-ap444:2.1_0": "Tamhi kāle sikhī buddho,",
  "tha-ap444:2.2_0": "eko adutiyo ahu;",
  "tha-ap444:2.3_0": "Mamassamaṁ upāgacchi,",
  "tha-ap444:2.4_0": "jānanto sabbakālikaṁ.",
  "tha-ap444:3.1_0": "Sakaṁ cittaṁ pasādetvā,",
  "tha-ap444:3.2_0": "vanditvāna ca subbataṁ;",
  "tha-ap444:3.3_0": "Ubho hatthehi paggayha,",
  "tha-ap444:3.4_0": "kolaṁ buddhassadāsahaṁ.",
  "tha-ap444:4.1_0": "Ekatiṁse ito kappe,",
  "tha-ap444:4.2_0": "yaṁ phalamadadiṁ tadā;",
  "tha-ap444:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap444:4.4_0": "koladānassidaṁ phalaṁ.",
  "tha-ap444:5.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap444:5.2_0": "…pe…",
  "tha-ap444:5.3_0": "viharāmi anāsavo.",
  "tha-ap444:6.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap444:6.2_0": "…pe…",
  "tha-ap444:6.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap444:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap444:7.2_0": "…pe…",
  "tha-ap444:7.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap444:8.1_0": "Itthaṁ sudaṁ āyasmā koladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap444:9.1_0": "Koladāyakattherassāpadānaṁ dutiyaṁ."
}