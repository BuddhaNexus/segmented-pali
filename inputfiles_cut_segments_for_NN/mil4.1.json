{
  "mil4.1:0.1_0": "Milindapañha",
  "mil4.1:0.2_0": "Meṇḍakapañhārambhakathā",
  "mil4.1:0.3_0": "Upāsakaguṇavagga",
  "mil4.1:0.4_0": "1. Aṭṭhamantaparivajjanīyaṭṭhāna",
  "mil4.1:1.1_0": "Bhassappavādo vetaṇḍī,",
  "mil4.1:1.2_0": "atibuddhi vicakkhaṇo;",
  "mil4.1:1.3_0": "Milindo ñāṇabhedāya,",
  "mil4.1:1.4_0": "nāgasenamupāgami.",
  "mil4.1:2.1_0": "Vasanto tassa chāyāya,",
  "mil4.1:2.2_0": "paripucchaṁ punappunaṁ;",
  "mil4.1:2.3_0": "Pabhinnabuddhi hutvāna,",
  "mil4.1:2.4_0": "sopi āsi tipeṭako.",
  "mil4.1:3.1_0": "Navaṅgaṁ anumajjanto,",
  "mil4.1:3.2_0": "rattibhāge rahogato;",
  "mil4.1:3.3_0": "Addakkhi meṇḍake pañhe,",
  "mil4.1:3.4_0": "dunniveṭhe saniggahe.",
  "mil4.1:4.1_0": "“Pariyāyabhāsitaṁ atthi,",
  "mil4.1:4.2_0": "atthi sandhāyabhāsitaṁ;",
  "mil4.1:4.3_0": "Sabhāvabhāsitaṁ atthi,",
  "mil4.1:4.4_0": "dhammarājassa sāsane.",
  "mil4.1:5.1_0": "Tesamatthaṁ aviññāya,",
  "mil4.1:5.2_0": "meṇḍake jinabhāsite;",
  "mil4.1:5.3_0": "Anāgatamhi addhāne,",
  "mil4.1:5.4_0": "viggaho tattha hessati.",
  "mil4.1:6.1_0": "Handa kathiṁ pasādetvā,",
  "mil4.1:6.2_0": "chejjāpessāmi meṇḍake;",
  "mil4.1:6.3_0": "Tassa niddiṭṭhamaggena,",
  "mil4.1:6.4_0": "niddisissantyanāgate”ti.",
  "mil4.1:7.1_0": "Atha kho milindo rājā pabhātāya rattiyā uddhaste aruṇe sīsaṁ nhatvā sirasi añjaliṁ paggahetvā atītānāgatapaccuppanne",
  "mil4.1:7.1_1": "sammāsambuddhe anussaritvā aṭṭha vattapadāni samādiyi “ito me anāgatāni satta divasāni aṭṭha guṇe samādiyitvā",
  "mil4.1:7.1_2": "tapo caritabbo bhavissati,",
  "mil4.1:7.1_3": "sohaṁ ciṇṇatapo samāno ācariyaṁ ārādhetvā meṇḍake pañhe pucchissāmī”ti.",
  "mil4.1:7.2_0": "Atha kho milindo rājā pakatidussayugaṁ apanetvā ābharaṇāni ca omuñcitvā kāsāvaṁ nivāsetvā muṇḍakapaṭisīsakaṁ",
  "mil4.1:7.2_1": "sīse paṭimuñcitvā munibhāvamupagantvā aṭṭha guṇe samādiyi “imaṁ sattāhaṁ mayā na rājattho anusāsitabbo,",
  "mil4.1:7.2_2": "na rāgūpasañhitaṁ cittaṁ uppādetabbaṁ,",
  "mil4.1:7.2_3": "na dosūpasañhitaṁ cittaṁ uppādetabbaṁ,",
  "mil4.1:7.2_4": "na mohūpasañhitaṁ cittaṁ uppādetabbaṁ,",
  "mil4.1:7.2_5": "dāsakammakaraporise janepi nivātavuttinā bhavitabbaṁ,",
  "mil4.1:7.2_6": "kāyikaṁ vācasikaṁ anurakkhitabbaṁ,",
  "mil4.1:7.2_7": "chapi āyatanāni niravasesato anurakkhitabbāni,",
  "mil4.1:7.2_8": "mettābhāvanāya mānasaṁ pakkhipitabban”ti.",
  "mil4.1:7.3_0": "Ime aṭṭha guṇe samādiyitvā tesveva aṭṭhasu guṇesu mānasaṁ patiṭṭhapetvā bahi anikkhamitvā sattāhaṁ vītināmetvā",
  "mil4.1:7.3_1": "aṭṭhame divase pabhātāya rattiyā pageva pātarāsaṁ katvā okkhittacakkhu mitabhāṇī susaṇṭhitena iriyāpathena",
  "mil4.1:7.3_2": "avikkhittena cittena haṭṭhena udaggena vippasannena theraṁ nāgasenaṁ upasaṅkamitvā therassa pāde sirasā",
  "mil4.1:7.3_3": "vanditvā ekamantaṁ ṭhito idamavoca—",
  "mil4.1:8.1_0": "“Atthi me,",
  "mil4.1:8.1_1": "bhante nāgasena,",
  "mil4.1:8.1_2": "koci attho tumhehi saddhiṁ mantayitabbo,",
  "mil4.1:8.1_3": "na tattha añño koci tatiyo icchitabbo,",
  "mil4.1:8.1_4": "suññe okāse pavivitte araññe aṭṭhaṅgupāgate samaṇasāruppe.",
  "mil4.1:8.2_0": "Tattha so pañho pucchitabbo bhavissati,",
  "mil4.1:8.2_1": "tattha me guyhaṁ na kātabbaṁ na rahassakaṁ,",
  "mil4.1:8.2_2": "arahāmahaṁ rahassakaṁ suṇituṁ sumantane upagate,",
  "mil4.1:8.2_3": "upamāyapi so attho upaparikkhitabbo,",
  "mil4.1:8.2_4": "yathā kiṁ viya,",
  "mil4.1:8.2_5": "yathā nāma,",
  "mil4.1:8.2_6": "bhante nāgasena,",
  "mil4.1:8.2_7": "mahāpathavī nikkhepaṁ arahati nikkhepe upagate.",
  "mil4.1:8.3_0": "Evameva kho, bhante nāgasena, arahāmahaṁ rahassakaṁ suṇituṁ sumantane upagate”ti.",
  "mil4.1:9.1_0": "Garunā saha pavivittapavanaṁ pavisitvā idamavoca—",
  "mil4.1:9.2_0": "“bhante nāgasena,",
  "mil4.1:9.2_1": "idha purisena mantayitukāmena aṭṭha ṭhānāni parivajjayitabbāni bhavanti,",
  "mil4.1:9.2_2": "na tesu ṭhānesu viññū puriso atthaṁ manteti,",
  "mil4.1:9.2_3": "mantitopi attho paripatati na sambhavati.",
  "mil4.1:9.3_0": "Katamāni aṭṭha ṭhānāni?",
  "mil4.1:9.4_0": "Visamaṭṭhānaṁ parivajjanīyaṁ,",
  "mil4.1:9.4_1": "sabhayaṁ parivajjanīyaṁ,",
  "mil4.1:9.4_2": "ativātaṭṭhānaṁ parivajjanīyaṁ,",
  "mil4.1:9.4_3": "paṭicchannaṭṭhānaṁ parivajjanīyaṁ,",
  "mil4.1:9.4_4": "devaṭṭhānaṁ parivajjanīyaṁ,",
  "mil4.1:9.4_5": "pantho parivajjanīyo,",
  "mil4.1:9.4_6": "saṅgāmo parivajjanīyo,",
  "mil4.1:9.4_7": "udakatitthaṁ parivajjanīyaṁ.",
  "mil4.1:9.5_0": "Imāni aṭṭha ṭhānāni parivajjanīyānī”ti.",
  "mil4.1:10.1_0": "Thero āha—",
  "mil4.1:10.2_0": "“ko doso visamaṭṭhāne, sabhaye, ativāte, paṭicchanne, devaṭṭhāne, panthe, saṅgāme, udakatitthe”ti?",
  "mil4.1:10.3_0": "“Visame,",
  "mil4.1:10.3_1": "bhante nāgasena,",
  "mil4.1:10.3_2": "mantito attho vikirati vidhamati paggharati na sambhavati,",
  "mil4.1:10.3_3": "sabhaye mano santassati,",
  "mil4.1:10.3_4": "santassito na sammā atthaṁ samanupassati,",
  "mil4.1:10.3_5": "ativāte saddo avibhūto hoti,",
  "mil4.1:10.3_6": "paṭicchanne upassutiṁ tiṭṭhanti,",
  "mil4.1:10.3_7": "devaṭṭhāne mantito attho garukaṁ pariṇamati,",
  "mil4.1:10.3_8": "panthe mantito attho tuccho bhavati,",
  "mil4.1:10.3_9": "saṅgāme cañcalo bhavati,",
  "mil4.1:10.3_10": "udakatitthe pākaṭo bhavati.",
  "mil4.1:10.4_0": "Bhavatīha—",
  "mil4.1:11.1_0": "‘Visamaṁ sabhayaṁ ativāto,",
  "mil4.1:11.2_0": "Paṭicchannaṁ devanissitaṁ;",
  "mil4.1:11.3_0": "Pantho ca saṅgāmo titthaṁ,",
  "mil4.1:11.4_0": "Aṭṭhete parivajjiyā’”ti.",
  "mil4.1:12.1_0": "Aṭṭha mantanassa parivajjanīyaṭṭhānāni."
}