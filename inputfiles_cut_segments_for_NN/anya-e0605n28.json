{
  "anya-e0605n28:0_0": "Pūjānidhānadīpanīgāthā",
  "anya-e0605n28:1_0": "189.",
  "anya-e0605n28:2_0": " Tasamā hi jātovarakamhi tassa,",
  "anya-e0605n28:3_0": "Āyattake maṅgalacakkavāḷe;",
  "anya-e0605n28:4_0": "Bhūtehi vatthūhi manoramehi,",
  "anya-e0605n28:5_0": "Pūjemi taṁ pūjitpūjitaṁ pure.",
  "anya-e0605n28:6_0": "190.",
  "anya-e0605n28:7_0": " Sohaṁ ajja panetasmiṁ cakkavāḷamhi pupphite,",
  "anya-e0605n28:8_0": "Thalaje jalaje vāpi sugandhe ca agandhake.",
  "anya-e0605n28:9_0": "191.",
  "anya-e0605n28:10_0": " Manussesu anekattha taḷākuyyānavāpisu,",
  "anya-e0605n28:11_0": "Pavane himavantasmiṁ tattha satta mahāsare.",
  "anya-e0605n28:12_0": "192.",
  "anya-e0605n28:13_0": " Parittadīpe dvisahasse mahādīpe supupphite,",
  "anya-e0605n28:14_0": "Sattaparibhaṇḍaselesu sinerupabbatuttame.",
  "anya-e0605n28:15_0": "193.",
  "anya-e0605n28:16_0": " Kumuduppalakādīni nāgānaṁ bhavanesupi,",
  "anya-e0605n28:17_0": "Pāṭalādīni pupphāni asurānaṁ hi ālaye.",
  "anya-e0605n28:18_0": "194.",
  "anya-e0605n28:19_0": " Koviḷārādikāni tu devatānaṁ hi ālaye,",
  "anya-e0605n28:20_0": "Evamādī anekattha pupphite dharaṇīruhe.",
  "anya-e0605n28:21_0": "195.",
  "anya-e0605n28:22_0": " Campakā salalā nimbā nāgapunnāgaketakā,",
  "anya-e0605n28:23_0": "Vassikā mallikā sālā koviḷārā ca pāṭali.",
  "anya-e0605n28:24_0": "196.",
  "anya-e0605n28:25_0": " Indīvarā asokā ca kaṇikārā ca makulā,",
  "anya-e0605n28:26_0": "Padumā puṇḍarikā ca sogandhikumuduppalā.",
  "anya-e0605n28:27_0": "197.",
  "anya-e0605n28:28_0": " Ete caññe ca rukkhā ca valliyo cāpi pupphitā,",
  "anya-e0605n28:29_0": "Sugandhā sukhasamphassā nānāvaṇṇanibhā subhā.",
  "anya-e0605n28:30_0": "198.",
  "anya-e0605n28:31_0": " Victrā nīlānekāni pītā lohitakāni ca,",
  "anya-e0605n28:32_0": "Kāḷā setā ca mañjaṭṭha nekavaṇṇā supupphitā.",
  "anya-e0605n28:33_0": "199.",
  "anya-e0605n28:34_0": " Sobhate pabbate heṭṭhā sarehi vanarājihi,",
  "anya-e0605n28:35_0": "Sandamānāhi gaṅgāhi himavā ratanākaro.",
  "anya-e0605n28:36_0": "200.",
  "anya-e0605n28:37_0": " Pattakiñjakkhareṇūhi okiṇṇaṁ hoti taṁ vanaṁ,",
  "anya-e0605n28:38_0": "Bhamarā pupphgandhehi samantā abhināditā.",
  "anya-e0605n28:39_0": "201.",
  "anya-e0605n28:40_0": " Athetta sakuṇā santi dijā mañjussarā subhā,",
  "anya-e0605n28:41_0": "Kūjantamupakūjanti utusampupphite dume.",
  "anya-e0605n28:42_0": "202.",
  "anya-e0605n28:43_0": " Niccharānaṁ nipātena pabbatā abhināditā,",
  "anya-e0605n28:44_0": "Pañcaṅgikāni tūriyāni dibbāni viya suyyare.",
  "anya-e0605n28:45_0": "203.",
  "anya-e0605n28:46_0": " Tattha naccanti tasmiṁ jalantaggisikhūpamā,",
  "anya-e0605n28:47_0": "Tasmiṁ hi kinnarā kiccaṁ padīpena karīyati.",
  "anya-e0605n28:48_0": "205.",
  "anya-e0605n28:49_0": " Muttājālāva dissanti niccharānaṁ hi pātakā,",
  "anya-e0605n28:50_0": "Pajjalantā va tiṭṭhanti maṇiveḷuriyādayo.",
  "anya-e0605n28:51_0": "206.",
  "anya-e0605n28:52_0": " Kāḷānusāri taggaraṁ kappūraṁ haricandanaṁ,",
  "anya-e0605n28:53_0": "Sakuṇānaṁ hi saddena mayūrānaṁ hi kekayā.",
  "anya-e0605n28:54_0": "207.",
  "anya-e0605n28:55_0": " Bhamarānaṁ hi ninnādā koñcanādena hatthinaṁ,",
  "anya-e0605n28:56_0": "Vijambhitena vāḷānaṁ kinnarānaṁ hi gītiyā;",
  "anya-e0605n28:57_0": "208.",
  "anya-e0605n28:58_0": " Pabbatānaṁ hi obhāsā maṇīnaṁ jotiyāpi ca,",
  "anya-e0605n28:59_0": "Vicitrabbhavitānehi dumānaṁ pupphadhūpiyā;",
  "anya-e0605n28:60_0": "Evaṁ sabbaṅgasampannaṁ kiṁ siyā nandanaṁ vanaṁ.",
  "anya-e0605n28:61_0": "209.",
  "anya-e0605n28:62_0": " Evaṁ susamphullavanaṁ hi yaṁ yaṁ,",
  "anya-e0605n28:63_0": "Tahiṁ tahiṁ pupphitapupphitaṁ subhaṁ;",
  "anya-e0605n28:64_0": "Mālaṁ susaddañca manuññagandhaṁ,",
  "anya-e0605n28:65_0": "Pūjemi taṁ pūjitapūjitaṁ purā.",
  "anya-e0605n28:66_0": "210.",
  "anya-e0605n28:67_0": " Nāgaloke manusse ca deve brahme ca yaṁ siyā,",
  "anya-e0605n28:68_0": "Sāmuddikaṁ bhūmigataṁ ākāsaṭṭhañca yaṁ dhamaṁ.",
  "anya-e0605n28:69_0": "211.",
  "anya-e0605n28:70_0": " Rajataṁ jātarūpañca muttā veḷuriyā maṇi,",
  "anya-e0605n28:71_0": "Masāragallaṁ phalikaṁ lohitaṅgaṁ pavāḷakaṁ.",
  "anya-e0605n28:72_0": "212.",
  "anya-e0605n28:73_0": " Yo so anantakappesu pūretvā dasapāramī,",
  "anya-e0605n28:74_0": "Buddho bodhesi sattānaṁ tassa pūjemi taṁ dhanaṁ.",
  "anya-e0605n28:75_0": "213.",
  "anya-e0605n28:76_0": " Khomaṁ koseyyaṁ kappāsaṁ sāṇaṁ bhaṅgañca kambalaṁ,",
  "anya-e0605n28:77_0": "Dukūlāni ca dibbāni dussāni vividhāni te.",
  "anya-e0605n28:78_0": "214.",
  "anya-e0605n28:79_0": " Anantavatthadānena hirottappādisaṁvaraṁ,",
  "anya-e0605n28:80_0": "Yassa siddhaṁ siyā tassa dussāni pujayāmahaṁ.",
  "anya-e0605n28:81_0": "215.",
  "anya-e0605n28:82_0": " Pavane jātarukkhānaṁ nānāphalarasuttamaṁ,",
  "anya-e0605n28:83_0": "Ambā kapiṭṭhā pansā cocamocādinappakā.",
  "anya-e0605n28:84_0": "216.",
  "anya-e0605n28:85_0": " Tasmiṁ gandharasaṁ ojaṁ buddhaseṭṭhassa pūjitaṁ,",
  "anya-e0605n28:86_0": "Vandāmi sirasā niccaṁ vippasannena cetasā.",
  "anya-e0605n28:87_0": "217.",
  "anya-e0605n28:88_0": " Pūjemi paṭhamaṁ tassa paṇidhānaṁ acintiyaṁ,",
  "anya-e0605n28:89_0": "Cakkavāḷamhi sabbehi vijjamānehi vatthuhi.",
  "anya-e0605n28:90_0": "218.",
  "anya-e0605n28:91_0": " Dasannaṁ pāramīnantu pūritaṭṭhānamuttamaṁ,",
  "anya-e0605n28:92_0": "Tato sālavane ramme jātaṭṭhānaṁ carimakaṁ.",
  "anya-e0605n28:93_0": "219.",
  "anya-e0605n28:94_0": " Chabbasāni padhānasmiṁ karaṇaṁ dukkarakārikaṁ,",
  "anya-e0605n28:95_0": "Apparājitapallaṅkaṁ buddhaṁ buddhaguṇaṁ name.",
  "anya-e0605n28:96_0": "220.",
  "anya-e0605n28:97_0": " Cuddasa buddhañāṇāni aṭṭharsa āveṇikaṁ,",
  "anya-e0605n28:98_0": "Pūjemi dasabalañāṇaṁ catuvesārajjamuttamaṁ.",
  "anya-e0605n28:99_0": "221.",
  "anya-e0605n28:100_0": " Āsayānusayañāṇaṁ indriyānaṁ paroparaṁ,",
  "anya-e0605n28:101_0": "Yamakapāṭihīrañca ñāṇaṁ sabbaññutaṁ pi ca.",
  "anya-e0605n28:102_0": "222.",
  "anya-e0605n28:103_0": " Mahākaruṇāpattiñāṇaṁ anāvaraṇmiti ca,",
  "anya-e0605n28:104_0": "Cha asādhāraṇānete ñatvāna pūjayāmahaṁ.",
  "anya-e0605n28:105_0": "223.",
  "anya-e0605n28:106_0": " Tato ca sattasattāhe dhammasammasitaṁ name,",
  "anya-e0605n28:107_0": "Brahmunā yācitaṭṭhānaṁ dhammaṁ desayituṁ varaṁ.",
  "anya-e0605n28:108_0": "224.",
  "anya-e0605n28:109_0": " Isipatane migadāye dhammacakkapavattanaṁ,",
  "anya-e0605n28:110_0": "Tato veḷuvanārāme vasitaṭhānañca pūjaye.",
  "anya-e0605n28:111_0": "225.",
  "anya-e0605n28:112_0": " Tato jetavanaṁ rammaṁ ciravutthaṁ mahesinā,",
  "anya-e0605n28:113_0": "Asādhāraṇamaññesaṁ yamakapāṭihariyaṁ.",
  "anya-e0605n28:114_0": "226.",
  "anya-e0605n28:115_0": " Pāricchattakamūlamhi abhidhammañca desanaṁ,",
  "anya-e0605n28:116_0": "Saṅkassanagaradvāre devorohaṇakaṁ pi ca.",
  "anya-e0605n28:117_0": "227.",
  "anya-e0605n28:118_0": " Tato ca himavantasmiṁ mahāsamayadesanaṁ,",
  "anya-e0605n28:119_0": "Vuttānetāni ṭhānāni natvāna pujayāmahaṁ.",
  "anya-e0605n28:120_0": "228.",
  "anya-e0605n28:121_0": " Caturāsītisahassehi dhammakkhandhehi saṅgahaṁ,",
  "anya-e0605n28:122_0": "Piṭakattayaṁ yathāvuttavidhinā pūjayāmahaṁ.",
  "anya-e0605n28:123_0": "229.",
  "anya-e0605n28:124_0": " Mārassa attano āyusaṅkhārosajjanaṁ name,",
  "anya-e0605n28:125_0": "Kusinārāya mallānaṁ yamakasālamantare.",
  "anya-e0605n28:126_0": "230.",
  "anya-e0605n28:127_0": " Paṇidhānamhi paṭṭhāya kataṁ kiccaṁ asesato,",
  "anya-e0605n28:128_0": "Niṭṭhapetvāna so sabbaṁ parinibbāyināsavo.",
  "anya-e0605n28:129_0": "231.",
  "anya-e0605n28:130_0": " Evaṁ nibbāyamānassa katakiccassa tādino,",
  "anya-e0605n28:131_0": "Ciragatā mahākaruṇā na nibbāyittha kiñcipi.",
  "anya-e0605n28:132_0": "232.",
  "anya-e0605n28:133_0": " Svāyaṁ dhammo vinayo ca desito sādhukaṁ mayā,",
  "anya-e0605n28:134_0": "Mamaccayena so satthā dhātu cāpi sarīrajā.",
  "anya-e0605n28:135_0": "233.",
  "anya-e0605n28:136_0": " Apparājitapallaṅkaṁ bodhirukkhañca uttamaṁ,",
  "anya-e0605n28:137_0": "Mamaccayena satthā ti anujāni mahāmuni.",
  "anya-e0605n28:138_0": "234.",
  "anya-e0605n28:139_0": " Mama ṭhane ṭhapetvāna dhātubodhiñca pūjitaṁ,",
  "anya-e0605n28:140_0": "Anujānāmi tumhākaṁ sādhanatthaṁ sivañjasaṁ.",
  "anya-e0605n28:141_0": "235.",
  "anya-e0605n28:142_0": " Tasmā hi tassa saddhammaṁ uggaṇhitvā yathātathaṁ,",
  "anya-e0605n28:143_0": "Yo deseti sambuddho ti natvāna pūjayāmahaṁ.",
  "anya-e0605n28:144_0": "236.",
  "anya-e0605n28:145_0": " Tasmā sāsapamattaṁ pi jinadhātuṁ asesiya,",
  "anya-e0605n28:146_0": "Vitthinnacakkavāḷamhi natvāna pūjayāmahaṁ.",
  "anya-e0605n28:147_0": "237.",
  "anya-e0605n28:148_0": " Paramparābhatānaṁ hi imamhā boddhirukkhato,",
  "anya-e0605n28:149_0": "Sabbesaṁ bodhirukkhānaṁ natvāna pūjayāmahaṁ.",
  "anya-e0605n28:150_0": "238.",
  "anya-e0605n28:151_0": " Yaṁ yaṁ paribhuñji bhagavā pattacīvaramādikaṁ,",
  "anya-e0605n28:152_0": "Sabbaṁ paribhogadhātuṁ natvāna pūjayāmahaṁ.",
  "anya-e0605n28:153_0": "239.",
  "anya-e0605n28:154_0": " Yattha katthaci sayito āsinno caṅkamepi vā,",
  "anya-e0605n28:155_0": "Pādalañchankaṁ katvā ṭhito natvāna pūjaye.",
  "anya-e0605n28:156_0": "240.",
  "anya-e0605n28:157_0": " Na sañjānanti ye buddhaṁ evarūpo ti ñātve,",
  "anya-e0605n28:158_0": "Kataṁ taṁ paṭimaṁ sabbaṁ natvāna pūjayāmahaṁ.",
  "anya-e0605n28:159_0": "241.",
  "anya-e0605n28:160_0": " Evaṁ buddhañca dhammañca saṇghañca anuttaraṁ,",
  "anya-e0605n28:161_0": "Cakkavāḷamhi sabbehi vatthūhi pūjayāmahaṁ."
}