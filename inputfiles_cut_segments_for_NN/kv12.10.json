{
  "kv12.10:0.1_0": "Kathāvatthu",
  "kv12.10:0.2_0": "Mahāpaṇṇāsaka",
  "kv12.10:0.3_0": "Dvādasamavagga",
  "kv12.10:0.4_0": "Sattamabhavikakathā",
  "kv12.10:1.1_0": "Na vattabbaṁ—",
  "kv12.10:1.2_0": "“sattamabhavikassa puggalassa pahīnā duggatī”ti?",
  "kv12.10:1.3_0": "Āmantā.",
  "kv12.10:1.4_0": "Sattamabhaviko puggalo nirayaṁ upapajjeyya, tiracchānayoniṁ upapajjeyya, pettivisayaṁ upapajjeyyāti?",
  "kv12.10:1.5_0": "Na hevaṁ vattabbe.",
  "kv12.10:1.6_0": "Tena hi sattamabhavikassa puggalassa pahīnā duggatīti.",
  "kv12.10:1.7_0": "Sattamabhavikakathā niṭṭhitā.",
  "kv12.10:1.8_0": "Bārasamo vaggo.",
  "kv12.10:1.9_0": "Tassuddānaṁ",
  "kv12.10:2.1_0": "Saṁvaro kammaṁ tatheva asaṁvaro,",
  "kv12.10:2.1_1": "sabbakammaṁ savipākaṁ,",
  "kv12.10:2.1_2": "saddo vipāko,",
  "kv12.10:2.1_3": "saḷāyatanaṁ vipāko,",
  "kv12.10:2.1_4": "sattakkhattuparamo puggalo sattakkhattuparamatāniyato,",
  "kv12.10:2.1_5": "kolaṅkolapuggalo kolaṅkolatāniyato,",
  "kv12.10:2.1_6": "ekabījī puggalo ekabījitāniyato,",
  "kv12.10:2.1_7": "diṭṭhisampanno puggalo sañcicca pāṇaṁ jīvitā voropeyya,",
  "kv12.10:2.1_8": "diṭṭhisampannassa puggalassa pahīnā duggati,",
  "kv12.10:2.1_9": "tatheva sattamabhavikassāti."
}