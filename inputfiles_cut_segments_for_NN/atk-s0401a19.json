{
  "atk-s0401a19:0_0": "20. Amatavaggavaṇṇanā",
  "atk-s0401a19:1_0": "600-611.Amataṁte,",
  "atk-s0401a19:1_1": "bhikkhave,",
  "atk-s0401a19:1_2": "na paribhuñjantīti maraṇavirahitaṁ nibbānaṁ na paribhuñjantīti attho.",
  "atk-s0401a19:1_3": "Nanu ca nibbānaṁ lokuttaraṁ,",
  "atk-s0401a19:1_4": "kāyagatāsati lokiyā,",
  "atk-s0401a19:1_5": "kathaṁ taṁ paribhuñjantā amataṁ paribhuñjantīti?",
  "atk-s0401a19:1_6": "Taṁ bhāvetvā adhigantabbato.",
  "atk-s0401a19:1_7": "Kāyagatañhi satiṁ bhāvento amatamadhigacchati,",
  "atk-s0401a19:1_8": "abhāvento nādhigacchati.",
  "atk-s0401a19:1_9": "Tasmā evaṁ vuttaṁ.",
  "atk-s0401a19:1_10": "Etenupāyena sabbattha attho veditabbo.",
  "atk-s0401a19:1_11": "Api cettha viraddhanti virādhitaṁ nādhigataṁ.",
  "atk-s0401a19:1_12": "Āraddhanti paripuṇṇaṁ.",
  "atk-s0401a19:1_13": "Pamādiṁsūti pamajjanti.",
  "atk-s0401a19:1_14": "Pamuṭṭhanti sammuṭṭhaṁ vissaritaṁ naṭṭhaṁ vā.",
  "atk-s0401a19:1_15": "Āsevitanti ādito sevitaṁ.",
  "atk-s0401a19:1_16": "Bhāvitanti vaḍḍhitaṁ.",
  "atk-s0401a19:1_17": "Bahulīkatanti punappunaṁ kataṁ.",
  "atk-s0401a19:1_18": "Anabhiññātanti ñātaabhiññāya ajānitaṁ.",
  "atk-s0401a19:1_19": "Apariññātanti ñātapariññāvaseneva apariññātaṁ.",
  "atk-s0401a19:1_20": "Asacchikatanti apaccakkhakataṁ.",
  "atk-s0401a19:1_21": "Sesaṁ sabbattha uttānatthamevāti.",
  "atk-s0401a19:2_0": "Amatavaggavaṇṇanā.",
  "atk-s0401a19:3_0": "Manorathapūraṇiyā aṅguttaranikāya-aṭṭhakathāya sahassasuttantaparimāṇassa",
  "atk-s0401a19:4_0": "Ekakanipātassa saṁvaṇṇanā niṭṭhitā."
}