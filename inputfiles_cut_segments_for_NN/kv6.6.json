{
  "kv6.6:0.1_0": "Kathāvatthu",
  "kv6.6:0.2_0": "Mahāpaṇṇāsaka",
  "kv6.6:0.3_0": "Chaṭṭhavagga",
  "kv6.6:0.4_0": "Ākāsakathā",
  "kv6.6:1.1_0": "Ākāso asaṅkhatoti?",
  "kv6.6:1.2_0": "Āmantā.",
  "kv6.6:1.3_0": "Nibbānaṁ tāṇaṁ leṇaṁ saraṇaṁ parāyanaṁ accutaṁ amatanti?",
  "kv6.6:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv6.6:1.5_0": "ākāso asaṅkhato, nibbānaṁ asaṅkhatanti?",
  "kv6.6:1.6_0": "Āmantā.",
  "kv6.6:1.7_0": "Dve asaṅkhatānīti?",
  "kv6.6:1.8_0": "Na hevaṁ vattabbe …pe….",
  "kv6.6:1.9_0": "Dve asaṅkhatānīti?",
  "kv6.6:1.10_0": "Āmantā.",
  "kv6.6:1.11_0": "Dve tāṇāni …pe… antarikā vāti?",
  "kv6.6:1.12_0": "Na hevaṁ vattabbe …pe….",
  "kv6.6:2.1_0": "Ākāso asaṅkhatoti?",
  "kv6.6:2.2_0": "Āmantā.",
  "kv6.6:2.3_0": "Atthi keci anākāsaṁ ākāsaṁ karontīti?",
  "kv6.6:2.4_0": "Āmantā.",
  "kv6.6:2.5_0": "Atthi keci saṅkhataṁ asaṅkhataṁ karontīti?",
  "kv6.6:2.6_0": "Na hevaṁ vattabbe …pe…",
  "kv6.6:2.7_0": "atthi keci ākāsaṁ anākāsaṁ karontīti?",
  "kv6.6:2.8_0": "Āmantā.",
  "kv6.6:2.9_0": "Atthi keci asaṅkhataṁ saṅkhataṁ karontīti?",
  "kv6.6:2.10_0": "Na hevaṁ vattabbe …pe….",
  "kv6.6:3.1_0": "Ākāse pakkhino gacchanti, candimasūriyā gacchanti, tārakarūpāni gacchanti, iddhiṁ vikubbanti, bāhuṁ cālenti, pāṇiṁ cālenti, leḍḍuṁ khipanti, laguḷaṁ khipanti, iddhiṁ khipanti, usuṁ khipantīti?",
  "kv6.6:3.2_0": "Āmantā.",
  "kv6.6:3.3_0": "Asaṅkhate pakkhino gacchanti, candimasūriyā gacchanti, tārakarūpāni gacchanti, iddhiṁ vikubbanti, bāhuṁ cālenti, pāṇiṁ cālenti, leḍḍuṁ khipanti, laguḷaṁ khipanti, iddhiṁ khipanti, usuṁ khipantīti?",
  "kv6.6:3.4_0": "Na hevaṁ vattabbe …pe….",
  "kv6.6:4.1_0": "Ākāsaṁ parivāretvā gharāni karonti koṭṭhāni karontīti?",
  "kv6.6:4.2_0": "Āmantā.",
  "kv6.6:4.3_0": "Asaṅkhataṁ parivāretvā gharāni karonti koṭṭhāni karontīti?",
  "kv6.6:4.4_0": "Na hevaṁ vattabbe …pe….",
  "kv6.6:5.1_0": "Udapāne khaññamāne anākāso ākāso hotīti?",
  "kv6.6:5.2_0": "Āmantā.",
  "kv6.6:5.3_0": "Saṅkhataṁ asaṅkhataṁ hotīti?",
  "kv6.6:5.4_0": "Na hevaṁ vattabbe …pe….",
  "kv6.6:6.1_0": "Tucchaudapāne pūriyamāne, tucchakoṭṭhe pūriyamāne,",
  "kv6.6:6.1_1": "tucchakumbhiyā pūriyamānāya ākāso antaradhāyatīti?",
  "kv6.6:6.2_0": "Āmantā.",
  "kv6.6:6.3_0": "Asaṅkhataṁ antaradhāyatīti?",
  "kv6.6:6.4_0": "Na hevaṁ vattabbe …pe….",
  "kv6.6:7.1_0": "Na vattabbaṁ—",
  "kv6.6:7.2_0": "“ākāso asaṅkhato”ti?",
  "kv6.6:7.3_0": "Āmantā.",
  "kv6.6:7.4_0": "Ākāso saṅkhatoti?",
  "kv6.6:7.5_0": "Na hevaṁ vattabbe …pe…",
  "kv6.6:7.6_0": "tena hi ākāso asaṅkhatoti.",
  "kv6.6:7.7_0": "Ākāsakathā niṭṭhitā."
}