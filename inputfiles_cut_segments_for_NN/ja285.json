{
  "ja285:0.1_0": "Jātaka",
  "ja285:0.2_0": "Tikanipāta",
  "ja285:0.3_0": "Abbhantaravagga",
  "ja285:0.4_0": "5. Maṇisūkarajātaka",
  "ja285:1.1_0": "“Dariyā satta vassāni,",
  "ja285:1.2_0": "tiṁsamattā vasāmase;",
  "ja285:1.3_0": "Haññāma maṇino ābhaṁ,",
  "ja285:1.4_0": "iti no mantanaṁ ahu.",
  "ja285:2.1_0": "Yāvatā maṇiṁ ghaṁsāma,",
  "ja285:2.2_0": "bhiyyo vodāyate maṇi;",
  "ja285:2.3_0": "Idañca dāni pucchāma,",
  "ja285:2.4_0": "kiṁ kiccaṁ idha maññasi”.",
  "ja285:3.1_0": "“Ayaṁ maṇi veḷūriyo,",
  "ja285:3.2_0": "akāco vimalo subho;",
  "ja285:3.3_0": "Nāssa sakkā siriṁ hantuṁ,",
  "ja285:3.4_0": "apakkamatha sūkarā”ti.",
  "ja285:4.1_0": "Maṇisūkarajātakaṁ pañcamaṁ."
}