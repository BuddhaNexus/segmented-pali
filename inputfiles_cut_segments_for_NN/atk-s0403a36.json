{
  "atk-s0403a36:0_0": "11. Tikavaggo",
  "atk-s0403a36:1_0": "1. Rāgasuttavaṇṇanā",
  "atk-s0403a36:2_0": "107.",
  "atk-s0403a36:2_1": "Ekādasamassa paṭhame asubhāti asubhakammaṭṭhānaṁ. Mettāti mettākammaṭṭhānaṁ.",
  "atk-s0403a36:2_2": "Paññāti sahavipassanā maggapaññā.",
  "atk-s0403a36:3_0": "6. Assādasuttavaṇṇanā",
  "atk-s0403a36:4_0": "112.",
  "atk-s0403a36:4_1": "Chaṭṭhe assādadiṭṭhīti sassatadiṭṭhi.",
  "atk-s0403a36:4_2": "Attānudiṭṭhīti attānaṁ anugatā vīsativatthukā sakkāyadiṭṭhi.",
  "atk-s0403a36:4_3": "Micchādiṭṭhīti dvāsaṭṭhividhāpi diṭṭhi.",
  "atk-s0403a36:4_4": "Sammādiṭṭhīti maggasammādiṭṭhi,",
  "atk-s0403a36:4_5": "natthi dinnantiādikā vā micchādiṭṭhi,",
  "atk-s0403a36:4_6": "kammassakatañāṇaṁ sammādiṭṭhi.",
  "atk-s0403a36:5_0": "7. Aratisuttavaṇṇanā",
  "atk-s0403a36:6_0": "113.",
  "atk-s0403a36:6_1": "Sattame adhammacariyāti dasa akusalakammapathā.",
  "atk-s0403a36:7_0": "10. Uddhaccasuttavaṇṇanā",
  "atk-s0403a36:8_0": "116.",
  "atk-s0403a36:8_1": "Dasame asaṁvaroti anadhivāsakabhāvo. Sesaṁ sabbattha uttānamevāti.",
  "atk-s0403a36:9_0": "Tikavaggo ekādasamo."
}