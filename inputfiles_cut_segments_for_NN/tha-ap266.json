{
  "tha-ap266:0.1_0": "Therāpadāna",
  "tha-ap266:0.2_0": "Padumukkhipavagga",
  "tha-ap266:0.3_0": "4 Padīpadāyakattheraapadāna",
  "tha-ap266:1.1_0": "“Devabhūto ahaṁ santo,",
  "tha-ap266:1.2_0": "oruyha pathaviṁ tadā;",
  "tha-ap266:1.3_0": "Padīpe pañca pādāsiṁ,",
  "tha-ap266:1.4_0": "pasanno sehi pāṇibhi.",
  "tha-ap266:2.1_0": "Catunnavutito kappe,",
  "tha-ap266:2.2_0": "yaṁ padīpamadaṁ tadā;",
  "tha-ap266:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap266:2.4_0": "dīpadānassidaṁ phalaṁ.",
  "tha-ap266:3.1_0": "Pañcapaññāsake kappe,",
  "tha-ap266:3.2_0": "eko āsiṁ mahīpati;",
  "tha-ap266:3.3_0": "Samantacakkhunāmena,",
  "tha-ap266:3.4_0": "cakkavattī mahabbalo.",
  "tha-ap266:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap266:4.2_0": "…pe…",
  "tha-ap266:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap266:5.1_0": "Itthaṁ sudaṁ āyasmā padīpadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap266:6.1_0": "Padīpadāyakattherassāpadānaṁ catutthaṁ."
}