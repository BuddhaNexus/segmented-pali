{
  "kv9.4:0.1_0": "Kathāvatthu",
  "kv9.4:0.2_0": "Mahāpaṇṇāsaka",
  "kv9.4:0.3_0": "Navamavagga",
  "kv9.4:0.4_0": "Anusayāanārammaṇātikathā",
  "kv9.4:1.1_0": "Anusayā anārammaṇāti?",
  "kv9.4:1.2_0": "Āmantā.",
  "kv9.4:1.3_0": "Rūpaṁ nibbānaṁ cakkhāyatanaṁ …pe…",
  "kv9.4:1.4_0": "phoṭṭhabbāyatananti?",
  "kv9.4:1.5_0": "Na hevaṁ vattabbe …pe….",
  "kv9.4:2.1_0": "Kāmarāgānusayo anārammaṇoti?",
  "kv9.4:2.2_0": "Āmantā.",
  "kv9.4:2.3_0": "Kāmarāgo kāmarāgapariyuṭṭhānaṁ kāmarāgasaṁyojanaṁ kāmogho kāmayogo kāmacchandanīvaraṇaṁ anārammaṇanti?",
  "kv9.4:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv9.4:2.5_0": "kāmarāgo kāmarāgapariyuṭṭhānaṁ kāmarāgasaṁyojanaṁ kāmogho kāmayogo kāmacchandanīvaraṇaṁ sārammaṇanti?",
  "kv9.4:2.6_0": "Āmantā.",
  "kv9.4:2.7_0": "Kāmarāgānusayo sārammaṇoti?",
  "kv9.4:2.8_0": "Na hevaṁ vattabbe …pe….",
  "kv9.4:3.1_0": "Kāmarāgānusayo anārammaṇoti?",
  "kv9.4:3.2_0": "Āmantā.",
  "kv9.4:3.3_0": "Katamakkhandhapariyāpannoti?",
  "kv9.4:3.4_0": "Saṅkhārakkhandhapariyāpannoti.",
  "kv9.4:3.5_0": "Saṅkhārakkhandho anārammaṇoti?",
  "kv9.4:3.6_0": "Na hevaṁ vattabbe …pe…",
  "kv9.4:3.7_0": "saṅkhārakkhandho anārammaṇoti?",
  "kv9.4:3.8_0": "Āmantā.",
  "kv9.4:3.9_0": "Vedanākkhandho saññākkhandho viññāṇakkhandho anārammaṇoti?",
  "kv9.4:3.10_0": "Na hevaṁ vattabbe …pe….",
  "kv9.4:4.1_0": "Kāmarāgānusayo saṅkhārakkhandhapariyāpanno anārammaṇoti?",
  "kv9.4:4.2_0": "Āmantā.",
  "kv9.4:4.3_0": "Kāmarāgo saṅkhārakkhandhapariyāpanno anārammaṇoti?",
  "kv9.4:4.4_0": "Na hevaṁ vattabbe …pe…",
  "kv9.4:4.5_0": "kāmarāgo saṅkhārakkhandhapariyāpanno sārammaṇoti?",
  "kv9.4:4.6_0": "Āmantā.",
  "kv9.4:4.7_0": "Kāmarāgānusayo saṅkhārakkhandhapariyāpanno sārammaṇoti?",
  "kv9.4:4.8_0": "Na hevaṁ vattabbe …pe….",
  "kv9.4:5.1_0": "Kāmarāgānusayo saṅkhārakkhandhapariyāpanno anārammaṇo,",
  "kv9.4:5.1_1": "kāmarāgo saṅkhārakkhandhapariyāpanno sārammaṇoti?",
  "kv9.4:5.2_0": "Āmantā.",
  "kv9.4:5.3_0": "Saṅkhārakkhandho ekadeso sārammaṇo ekadeso anārammaṇoti?",
  "kv9.4:5.4_0": "Na hevaṁ vattabbe …pe…",
  "kv9.4:5.5_0": "saṅkhārakkhandho ekadeso sārammaṇo ekadeso anārammaṇoti?",
  "kv9.4:5.6_0": "Āmantā.",
  "kv9.4:5.7_0": "Vedanākkhandho saññākkhandho viññāṇakkhandho ekadeso sārammaṇo ekadeso anārammaṇoti?",
  "kv9.4:5.8_0": "Na hevaṁ vattabbe …pe….",
  "kv9.4:6.1_0": "Paṭighānusayo mānānusayo diṭṭhānusayo vicikicchānusayo bhavarāgānusayo avijjānusayo anārammaṇoti?",
  "kv9.4:6.2_0": "Āmantā.",
  "kv9.4:6.3_0": "Avijjā avijjogho avijjāyogo avijjānusayo avijjāpariyuṭṭhānaṁ avijjāsaṁyojanaṁ avijjānīvaraṇaṁ anārammaṇanti?",
  "kv9.4:6.4_0": "Na hevaṁ vattabbe …pe…",
  "kv9.4:6.5_0": "avijjā avijjogho …pe…",
  "kv9.4:6.6_0": "avijjānīvaraṇaṁ sārammaṇanti?",
  "kv9.4:6.7_0": "Āmantā.",
  "kv9.4:6.8_0": "Avijjānusayo sārammaṇoti?",
  "kv9.4:6.9_0": "Na hevaṁ vattabbe …pe….",
  "kv9.4:7.1_0": "Avijjānusayo anārammaṇoti?",
  "kv9.4:7.2_0": "Āmantā.",
  "kv9.4:7.3_0": "Katamakkhandhapariyāpannoti?",
  "kv9.4:7.4_0": "Saṅkhārakkhandhapariyāpannoti.",
  "kv9.4:7.5_0": "Saṅkhārakkhandho anārammaṇoti?",
  "kv9.4:7.6_0": "Na hevaṁ vattabbe …pe…",
  "kv9.4:7.7_0": "saṅkhārakkhandho anārammaṇoti?",
  "kv9.4:7.8_0": "Āmantā.",
  "kv9.4:7.9_0": "Vedanākkhandho saññākkhandho viññāṇakkhandho anārammaṇoti?",
  "kv9.4:7.10_0": "Na hevaṁ vattabbe …pe….",
  "kv9.4:8.1_0": "Avijjānusayo saṅkhārakkhandhapariyāpanno anārammaṇoti?",
  "kv9.4:8.2_0": "Āmantā.",
  "kv9.4:8.3_0": "Avijjā saṅkhārakkhandhapariyāpannā anārammaṇāti?",
  "kv9.4:8.4_0": "Na hevaṁ vattabbe …pe…",
  "kv9.4:8.5_0": "avijjā saṅkhārakkhandhapariyāpannā sārammaṇāti?",
  "kv9.4:8.6_0": "Āmantā.",
  "kv9.4:8.7_0": "Avijjānusayo saṅkhārakkhandhapariyāpanno sārammaṇoti?",
  "kv9.4:8.8_0": "Na hevaṁ vattabbe …pe….",
  "kv9.4:9.1_0": "Avijjānusayo saṅkhārakkhandhapariyāpanno anārammaṇo,",
  "kv9.4:9.1_1": "avijjā saṅkhārakkhandhapariyāpannā sārammaṇāti?",
  "kv9.4:9.2_0": "Āmantā.",
  "kv9.4:9.3_0": "Saṅkhārakkhandho ekadeso sārammaṇo ekadeso anārammaṇoti?",
  "kv9.4:9.4_0": "Na hevaṁ vattabbe …pe…",
  "kv9.4:9.5_0": "saṅkhārakkhandho ekadeso sārammaṇo ekadeso anārammaṇoti?",
  "kv9.4:9.6_0": "Āmantā.",
  "kv9.4:9.7_0": "Vedanākkhandho saññākkhandho viññāṇakkhandho ekadeso sārammaṇo ekadeso anārammaṇoti?",
  "kv9.4:9.8_0": "Na hevaṁ vattabbe …pe….",
  "kv9.4:10.1_0": "Na vattabbaṁ—",
  "kv9.4:10.2_0": "“anusayā anārammaṇā”ti?",
  "kv9.4:10.3_0": "Āmantā.",
  "kv9.4:10.4_0": "Puthujjano kusalābyākate citte vattamāne sānusayoti vattabboti?",
  "kv9.4:10.5_0": "Āmantā.",
  "kv9.4:10.6_0": "Atthi tesaṁ anusayānaṁ ārammaṇanti?",
  "kv9.4:10.7_0": "Na hevaṁ vattabbe …pe…",
  "kv9.4:10.8_0": "tena hi anusayā anārammaṇāti.",
  "kv9.4:10.9_0": "Puthujjano kusalābyākate citte vattamāne sarāgoti vattabboti?",
  "kv9.4:10.10_0": "Āmantā.",
  "kv9.4:10.11_0": "Atthi tassa rāgassa ārammaṇanti?",
  "kv9.4:10.12_0": "Na hevaṁ vattabbe …pe….",
  "kv9.4:10.13_0": "Tena hi rāgo anārammaṇoti.",
  "kv9.4:10.14_0": "Anusayā anārammaṇātikathā niṭṭhitā."
}