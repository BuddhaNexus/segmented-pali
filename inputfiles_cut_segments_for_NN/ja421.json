{
  "ja421:0.1_0": "Jātaka",
  "ja421:0.2_0": "Aṭṭhakanipāta",
  "ja421:0.3_0": "Kaccānivagga",
  "ja421:0.4_0": "5. Gaṅgamālajātaka",
  "ja421:1.1_0": "“Aṅgārajātā pathavī,",
  "ja421:1.2_0": "kukkuḷānugatā mahī;",
  "ja421:1.3_0": "Atha gāyasi vattāni,",
  "ja421:1.4_0": "na taṁ tapati ātapo.",
  "ja421:2.1_0": "Uddhaṁ tapati ādicco,",
  "ja421:2.2_0": "adho tapati vālukā;",
  "ja421:2.3_0": "Atha gāyasi vattāni,",
  "ja421:2.4_0": "na taṁ tapati ātapo”.",
  "ja421:3.1_0": "“Na maṁ tapati ātapo,",
  "ja421:3.2_0": "ātapā tapayanti maṁ;",
  "ja421:3.3_0": "Atthā hi vividhā rāja,",
  "ja421:3.4_0": "te tapanti na ātapo”.",
  "ja421:4.1_0": "“Addasaṁ kāma te mūlaṁ,",
  "ja421:4.2_0": "saṅkappā kāma jāyasi;",
  "ja421:4.3_0": "Na taṁ saṅkappayissāmi,",
  "ja421:4.4_0": "evaṁ kāma na hehisi”.",
  "ja421:5.1_0": "“Appāpi kāmā na alaṁ,",
  "ja421:5.2_0": "bahūhipi na tappati;",
  "ja421:5.3_0": "Ahahā bālalapanā,",
  "ja421:5.4_0": "parivajjetha jaggato”.",
  "ja421:6.1_0": "“Appassa kammassa phalaṁ mamedaṁ,",
  "ja421:6.2_0": "Udayo ajjhāgamā mahattapattaṁ;",
  "ja421:6.3_0": "Suladdhalābho vata māṇavassa,",
  "ja421:6.4_0": "Yo pabbajī kāmarāgaṁ pahāya”.",
  "ja421:7.1_0": "“Tapasā pajahanti pāpakammaṁ,",
  "ja421:7.2_0": "Tapasā nhāpitakumbhakārabhāvaṁ;",
  "ja421:7.3_0": "Tapasā abhibhuyya gaṅgamāla,",
  "ja421:7.4_0": "Nāmenālapasajja brahmadattaṁ”.",
  "ja421:8.1_0": "“Sandiṭṭhikameva ‘amma’ passatha,",
  "ja421:8.2_0": "Khantīsoraccassa ayaṁ vipāko;",
  "ja421:8.3_0": "Yo sabbajanassa vanditohu,",
  "ja421:8.4_0": "Taṁ vandāma sarājikā samaccā”.",
  "ja421:9.1_0": "“Mā kiñci avacuttha gaṅgamālaṁ,",
  "ja421:9.2_0": "Muninaṁ monapathesu sikkhamānaṁ;",
  "ja421:9.3_0": "Eso hi atari aṇṇavaṁ,",
  "ja421:9.4_0": "Yaṁ taritvā caranti vītasokā”ti.",
  "ja421:10.1_0": "Gaṅgamālajātakaṁ pañcamaṁ."
}