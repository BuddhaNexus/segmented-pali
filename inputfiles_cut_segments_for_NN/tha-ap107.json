{
  "tha-ap107:0.1_0": "Therāpadāna",
  "tha-ap107:0.2_0": "Bhikkhadāyivagga",
  "tha-ap107:0.3_0": "5. Muṭṭhipupphiyattheraapadāna",
  "tha-ap107:1.1_0": "“Sudassanoti nāmena,",
  "tha-ap107:1.2_0": "mālākāro ahaṁ tadā;",
  "tha-ap107:1.3_0": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap107:1.4_0": "lokajeṭṭhaṁ narāsabhaṁ.",
  "tha-ap107:2.1_0": "Jātipupphaṁ gahetvāna,",
  "tha-ap107:2.2_0": "pūjayiṁ padumuttaraṁ;",
  "tha-ap107:2.3_0": "Visuddhacakkhu sumano,",
  "tha-ap107:2.4_0": "dibbacakkhuṁ samajjhagaṁ.",
  "tha-ap107:3.1_0": "Etissā pupphapūjāya,",
  "tha-ap107:3.2_0": "cittassa paṇidhīhi ca;",
  "tha-ap107:3.3_0": "Kappānaṁ satasahassaṁ,",
  "tha-ap107:3.4_0": "duggatiṁ nupapajjahaṁ.",
  "tha-ap107:4.1_0": "Soḷasāsiṁsu rājāno,",
  "tha-ap107:4.2_0": "devuttarasanāmakā;",
  "tha-ap107:4.3_0": "Chattiṁsamhi ito kappe,",
  "tha-ap107:4.4_0": "cakkavattī mahabbalā.",
  "tha-ap107:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap107:5.2_0": "…pe…",
  "tha-ap107:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap107:6.1_0": "Itthaṁ sudaṁ āyasmā muṭṭhipupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap107:7.1_0": "Muṭṭhipupphiyattherassāpadānaṁ pañcamaṁ."
}