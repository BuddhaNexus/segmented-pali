{
  "mil6.3.10:0.1_0": "Milindapañha",
  "mil6.3.10:0.2_0": "Anumānapañha",
  "mil6.3.10:0.3_0": "Vessantaravagga",
  "mil6.3.10:0.4_0": "10. Nibbānarūpasaṇṭhānapañha",
  "mil6.3.10:1.1_0": "“Bhante nāgasena,",
  "mil6.3.10:1.1_1": "‘nibbānaṁ nibbānan’ti yaṁ vadesi,",
  "mil6.3.10:1.1_2": "sakkā pana tassa nibbānassa rūpaṁ vā saṇṭhānaṁ vā vayaṁ vā pamāṇaṁ vā opammena vā kāraṇena vā hetunā vā nayena vā upadassayitun”ti?",
  "mil6.3.10:1.2_0": "“Appaṭibhāgaṁ, mahārāja, nibbānaṁ,",
  "mil6.3.10:1.2_1": "na sakkā nibbānassa rūpaṁ vā saṇṭhānaṁ vā vayaṁ vā pamāṇaṁ vā opammena vā kāraṇena vā hetunā vā nayena vā upadassayitun”ti.",
  "mil6.3.10:1.3_0": "“Etampāhaṁ, bhante nāgasena, na sampaṭicchāmi,",
  "mil6.3.10:1.3_1": "yaṁ atthidhammassa nibbānassa rūpaṁ vā saṇṭhānaṁ vā vayaṁ vā pamāṇaṁ vā opammena vā kāraṇena vā hetunā vā nayena vā apaññāpanaṁ, kāraṇena maṁ saññāpehī”ti.",
  "mil6.3.10:1.4_0": "“Hotu, mahārāja, kāraṇena taṁ saññāpessāmi.",
  "mil6.3.10:1.5_0": "Atthi, mahārāja, mahāsamuddo nāmā”ti?",
  "mil6.3.10:1.6_0": "“Āma, bhante, attheso mahāsamuddo”ti.",
  "mil6.3.10:1.7_0": "“Sace taṁ, mahārāja,",
  "mil6.3.10:1.7_1": "koci evaṁ puccheyya ‘kittakaṁ, mahārāja, mahāsamudde udakaṁ, kati pana te sattā,",
  "mil6.3.10:1.7_2": "ye mahāsamudde paṭivasantī’ti, evaṁ puṭṭho tvaṁ, mahārāja, kinti tassa byākareyyāsī”ti?",
  "mil6.3.10:1.8_0": "“Sace maṁ, bhante,",
  "mil6.3.10:1.8_1": "koci evaṁ puccheyya ‘kittakaṁ, mahārāja, mahāsamudde udakaṁ, kati pana te sattā,",
  "mil6.3.10:1.8_2": "ye mahāsamudde paṭivasantī’ti, tamahaṁ, bhante,",
  "mil6.3.10:1.8_3": "evaṁ vadeyyaṁ ‘apucchitabbaṁ maṁ tvaṁ, ambho purisa, pucchasi,",
  "mil6.3.10:1.8_4": "nesā pucchā kenaci pucchitabbā, ṭhapanīyo eso pañho.",
  "mil6.3.10:1.9_0": "Avibhatto lokakkhāyikehi mahāsamuddo,",
  "mil6.3.10:1.9_1": "na sakkā mahāsamudde udakaṁ pariminituṁ sattā vā ye tattha vāsamupagatāti evāhaṁ,",
  "mil6.3.10:1.9_2": "bhante tassa paṭivacanaṁ dadeyyan’”ti.",
  "mil6.3.10:2.1_0": "“Kissa pana tvaṁ, mahārāja,",
  "mil6.3.10:2.1_1": "atthidhamme mahāsamudde evaṁ paṭivacanaṁ dadeyyāsi,",
  "mil6.3.10:2.1_2": "nanu vigaṇetvā tassa ācikkhitabbaṁ ‘ettakaṁ mahāsamudde udakaṁ,",
  "mil6.3.10:2.1_3": "ettakā ca sattā mahāsamudde paṭivasantī’”ti?",
  "mil6.3.10:2.2_0": "“Na sakkā, bhante, avisayo eso pañho”ti.",
  "mil6.3.10:3.1_0": "“Yathā, mahārāja,",
  "mil6.3.10:3.1_1": "atthidhammeyeva mahāsamudde na sakkā udakaṁ parigaṇetuṁ sattā vā ye tattha vāsamupagatā;",
  "mil6.3.10:3.2_0": "evameva kho, mahārāja,",
  "mil6.3.10:3.2_1": "atthidhammasseva nibbānassa na sakkā rūpaṁ vā saṇṭhānaṁ vā vayaṁ vā pamāṇaṁ vā opammena vā kāraṇena vā hetunā vā nayena vā upadassayituṁ, vigaṇeyya, mahārāja,",
  "mil6.3.10:3.2_2": "iddhimā cetovasippatto mahāsamudde udakaṁ tatrāsaye ca satte,",
  "mil6.3.10:3.2_3": "na tveva so iddhimā cetovasippatto sakkuṇeyya nibbānassa rūpaṁ vā saṇṭhānaṁ vā vayaṁ vā pamāṇaṁ vā opammena vā kāraṇena vā hetunā vā nayena vā upadassayituṁ.",
  "mil6.3.10:4.1_0": "Aparampi, mahārāja, uttariṁ kāraṇaṁ suṇohi,",
  "mil6.3.10:4.1_1": "atthidhammasseva nibbānassa na sakkā rūpaṁ vā saṇṭhānaṁ vā vayaṁ vā pamāṇaṁ vā opammena vā kāraṇena vā hetunā vā nayena vā upadassayitunti.",
  "mil6.3.10:4.2_0": "Atthi, mahārāja, devesu arūpakāyikā nāma devā”ti.",
  "mil6.3.10:4.3_0": "“Āma, bhante,",
  "mil6.3.10:4.3_1": "suyyati ‘atthi devesu arūpakāyikā nāma devā’”ti.",
  "mil6.3.10:4.4_0": "“Sakkā pana, mahārāja,",
  "mil6.3.10:4.4_1": "tesaṁ arūpakāyikānaṁ devānaṁ rūpaṁ vā saṇṭhānaṁ vā vayaṁ vā pamāṇaṁ vā opammena vā kāraṇena vā hetunā vā nayena vā upadassayitun”ti?",
  "mil6.3.10:4.5_0": "“Na hi, bhante”ti.",
  "mil6.3.10:4.6_0": "“Tena hi, mahārāja, natthi arūpakāyikā devā”ti?",
  "mil6.3.10:4.7_0": "“Atthi, bhante, arūpakāyikā devā,",
  "mil6.3.10:4.7_1": "na ca sakkā tesaṁ rūpaṁ vā saṇṭhānaṁ vā vayaṁ vā pamāṇaṁ vā opammena vā kāraṇena vā hetunā vā nayena vā upadassayitun”ti.",
  "mil6.3.10:4.8_0": "“Yathā, mahārāja,",
  "mil6.3.10:4.8_1": "atthisattānaṁyeva arūpakāyikānaṁ devānaṁ na sakkā rūpaṁ vā saṇṭhānaṁ vā vayaṁ vā pamāṇaṁ vā opammena vā kāraṇena vā hetunā vā nayena vā upadassayituṁ;",
  "mil6.3.10:4.9_0": "evameva kho, mahārāja,",
  "mil6.3.10:4.9_1": "atthidhammasseva nibbānassa na sakkā rūpaṁ vā saṇṭhānaṁ vā vayaṁ vā pamāṇaṁ vā opammena vā kāraṇena vā hetunā vā nayena vā upadassayitun”ti.",
  "mil6.3.10:5.1_0": "“Bhante nāgasena, hotu ekantasukhaṁ nibbānaṁ,",
  "mil6.3.10:5.1_1": "na ca sakkā tassa rūpaṁ vā saṇṭhānaṁ vā vayaṁ vā pamāṇaṁ vā opammena vā kāraṇena vā hetunā vā nayena vā upadassayituṁ.",
  "mil6.3.10:5.2_0": "Atthi pana, bhante,",
  "mil6.3.10:5.2_1": "nibbānassa guṇaṁ aññehi anupaviṭṭhaṁ kiñci opammanidassanamattan”ti?",
  "mil6.3.10:5.3_0": "“Sarūpato, mahārāja, natthi,",
  "mil6.3.10:5.3_1": "guṇato pana sakkā kiñci opammanidassanamattaṁ upadassayitun”ti.",
  "mil6.3.10:5.4_0": "“Sādhu, bhante nāgasena,",
  "mil6.3.10:5.4_1": "yathāhaṁ labhāmi nibbānassa guṇatopi ekadesaparidīpanamattaṁ, tathā sīghaṁ brūhi,",
  "mil6.3.10:5.4_2": "nibbāpehi me hadayapariḷāhaṁ vinaya sītalamadhuravacanamālutenā”ti.",
  "mil6.3.10:6.1_0": "“Padumassa, mahārāja,",
  "mil6.3.10:6.1_1": "eko guṇo nibbānaṁ anupaviṭṭho, udakassa dve guṇā, agadassa tayo guṇā, mahāsamuddassa cattāro guṇā, bhojanassa pañca guṇā, ākāsassa dasa guṇā, maṇiratanassa tayo guṇā, lohitacandanassa tayo guṇā, sappimaṇḍassa tayo guṇā,",
  "mil6.3.10:6.1_2": "girisikharassa pañca guṇā nibbānaṁ anupaviṭṭhā”ti.",
  "mil6.3.10:7.1_0": "“Bhante nāgasena,",
  "mil6.3.10:7.1_1": "‘padumassa eko guṇo nibbānaṁ anupaviṭṭho’ti yaṁ vadesi,",
  "mil6.3.10:7.1_2": "katamo padumassa eko guṇo nibbānaṁ anupaviṭṭho”ti?",
  "mil6.3.10:7.2_0": "“Yathā, mahārāja, padumaṁ anupalittaṁ udakena;",
  "mil6.3.10:7.3_0": "evameva kho, mahārāja,",
  "mil6.3.10:7.3_1": "nibbānaṁ sabbakilesehi anupalittaṁ.",
  "mil6.3.10:7.4_0": "Ayaṁ, mahārāja,",
  "mil6.3.10:7.4_1": "padumassa eko guṇo nibbānaṁ anupaviṭṭho”ti.",
  "mil6.3.10:8.1_0": "“Bhante nāgasena,",
  "mil6.3.10:8.1_1": "‘udakassa dve guṇā nibbānaṁ anupaviṭṭhā’ti yaṁ vadesi,",
  "mil6.3.10:8.1_2": "katame udakassa dve guṇā nibbānaṁ anupaviṭṭhā”ti?",
  "mil6.3.10:8.2_0": "“Yathā, mahārāja,",
  "mil6.3.10:8.2_1": "udakaṁ sītalaṁ pariḷāhanibbāpanaṁ;",
  "mil6.3.10:8.3_0": "evameva kho, mahārāja,",
  "mil6.3.10:8.3_1": "nibbānaṁ sītalaṁ sabbakilesapariḷāhanibbāpanaṁ.",
  "mil6.3.10:8.4_0": "Ayaṁ, mahārāja,",
  "mil6.3.10:8.4_1": "udakassa paṭhamo guṇo nibbānaṁ anupaviṭṭho.",
  "mil6.3.10:8.5_0": "Puna caparaṁ, mahārāja,",
  "mil6.3.10:8.5_1": "udakaṁ kilantatasitapipāsitaghammābhitattānaṁ janapasupajānaṁ pipāsāvinayanaṁ;",
  "mil6.3.10:8.6_0": "evameva kho, mahārāja,",
  "mil6.3.10:8.6_1": "nibbānaṁ kāmataṇhābhavataṇhāvibhavataṇhāpipāsāvinayanaṁ.",
  "mil6.3.10:8.7_0": "Ayaṁ, mahārāja,",
  "mil6.3.10:8.7_1": "udakassa dutiyo guṇo nibbānaṁ anupaviṭṭho.",
  "mil6.3.10:8.8_0": "Ime kho, mahārāja,",
  "mil6.3.10:8.8_1": "udakassa dve guṇā nibbānaṁ anupaviṭṭhā”ti.",
  "mil6.3.10:9.1_0": "“Bhante nāgasena,",
  "mil6.3.10:9.1_1": "‘agadassa tayo guṇā nibbānaṁ anupaviṭṭhā’ti yaṁ vadesi,",
  "mil6.3.10:9.1_2": "katame agadassa tayo guṇā nibbānaṁ anupaviṭṭhā”ti?",
  "mil6.3.10:9.2_0": "“Yathā, mahārāja,",
  "mil6.3.10:9.2_1": "agado visapīḷitānaṁ sattānaṁ paṭisaraṇaṁ;",
  "mil6.3.10:9.3_0": "evameva kho, mahārāja,",
  "mil6.3.10:9.3_1": "nibbānaṁ kilesavisapīḷitānaṁ sattānaṁ paṭisaraṇaṁ.",
  "mil6.3.10:9.4_0": "Ayaṁ, mahārāja,",
  "mil6.3.10:9.4_1": "agadassa paṭhamo guṇo nibbānaṁ anupaviṭṭho.",
  "mil6.3.10:9.5_0": "Puna caparaṁ, mahārāja, agado rogānaṁ antakaro;",
  "mil6.3.10:9.6_0": "evameva kho, mahārāja,",
  "mil6.3.10:9.6_1": "nibbānaṁ sabbadukkhānaṁ antakaraṁ.",
  "mil6.3.10:9.7_0": "Ayaṁ, mahārāja,",
  "mil6.3.10:9.7_1": "agadassa dutiyo guṇo nibbānaṁ anupaviṭṭho.",
  "mil6.3.10:9.8_0": "Puna caparaṁ, mahārāja, agado amataṁ;",
  "mil6.3.10:9.9_0": "evameva kho, mahārāja, nibbānaṁ amataṁ.",
  "mil6.3.10:9.10_0": "Ayaṁ, mahārāja,",
  "mil6.3.10:9.10_1": "agadassa tatiyo guṇo nibbānaṁ anupaviṭṭho.",
  "mil6.3.10:9.11_0": "Ime kho, mahārāja,",
  "mil6.3.10:9.11_1": "agadassa tayo guṇā nibbānaṁ anupaviṭṭhā”ti.",
  "mil6.3.10:10.1_0": "“Bhante nāgasena,",
  "mil6.3.10:10.1_1": "‘mahāsamuddassa cattāro guṇā nibbānaṁ anupaviṭṭhā’ti yaṁ vadesi,",
  "mil6.3.10:10.1_2": "katame mahāsamuddassa cattāro guṇā nibbānaṁ anupaviṭṭhā”ti?",
  "mil6.3.10:10.2_0": "“Yathā, mahārāja, mahāsamuddo suñño sabbakuṇapehi;",
  "mil6.3.10:10.3_0": "evameva kho, mahārāja,",
  "mil6.3.10:10.3_1": "nibbānaṁ suññaṁ sabbakilesakuṇapehi.",
  "mil6.3.10:10.4_0": "Ayaṁ, mahārāja,",
  "mil6.3.10:10.4_1": "mahāsamuddassa paṭhamo guṇo nibbānaṁ anupaviṭṭho.",
  "mil6.3.10:10.5_0": "Puna caparaṁ, mahārāja,",
  "mil6.3.10:10.5_1": "mahāsamuddo mahanto anorapāro, na paripūrati sabbasavantīhi;",
  "mil6.3.10:10.6_0": "evameva kho, mahārāja,",
  "mil6.3.10:10.6_1": "nibbānaṁ mahantaṁ anorapāraṁ, na pūrati sabbasattehi.",
  "mil6.3.10:10.7_0": "Ayaṁ, mahārāja,",
  "mil6.3.10:10.7_1": "mahāsamuddassa dutiyo guṇo nibbānaṁ anupaviṭṭho.",
  "mil6.3.10:10.8_0": "Puna caparaṁ, mahārāja,",
  "mil6.3.10:10.8_1": "mahāsamuddo mahantānaṁ bhūtānaṁ āvāso;",
  "mil6.3.10:10.9_0": "evameva kho, mahārāja,",
  "mil6.3.10:10.9_1": "nibbānaṁ mahantānaṁ arahantānaṁ vimalakhīṇāsavabalappattavasībhūtamahābhūtānaṁ āvāso.",
  "mil6.3.10:10.10_0": "Ayaṁ, mahārāja,",
  "mil6.3.10:10.10_1": "mahāsamuddassa tatiyo guṇo nibbānaṁ anupaviṭṭho.",
  "mil6.3.10:10.11_0": "Puna caparaṁ, mahārāja,",
  "mil6.3.10:10.11_1": "mahāsamuddo aparimitavividhavipulavīcipupphasaṅkusumito;",
  "mil6.3.10:10.12_0": "evameva kho, mahārāja,",
  "mil6.3.10:10.12_1": "nibbānaṁ aparimitavividhavipulaparisuddhavijjāvimuttipupphasaṅkusumitaṁ.",
  "mil6.3.10:10.13_0": "Ayaṁ, mahārāja,",
  "mil6.3.10:10.13_1": "mahāsamuddassa catuttho guṇo nibbānaṁ anupaviṭṭho.",
  "mil6.3.10:10.14_0": "Ime kho, mahārāja,",
  "mil6.3.10:10.14_1": "mahāsamuddassa cattāro guṇā nibbānaṁ anupaviṭṭhā”ti.",
  "mil6.3.10:11.1_0": "“Bhante nāgasena,",
  "mil6.3.10:11.1_1": "‘bhojanassa pañca guṇā nibbānaṁ anupaviṭṭhā’ti yaṁ vadesi,",
  "mil6.3.10:11.1_2": "katame bhojanassa pañca guṇā nibbānaṁ anupaviṭṭhā”ti?",
  "mil6.3.10:11.2_0": "“Yathā, mahārāja,",
  "mil6.3.10:11.2_1": "bhojanaṁ sabbasattānaṁ āyudhāraṇaṁ;",
  "mil6.3.10:11.3_0": "evameva kho, mahārāja,",
  "mil6.3.10:11.3_1": "nibbānaṁ sacchikataṁ jarāmaraṇanāsanato āyudhāraṇaṁ.",
  "mil6.3.10:11.4_0": "Ayaṁ, mahārāja,",
  "mil6.3.10:11.4_1": "bhojanassa paṭhamo guṇo nibbānaṁ anupaviṭṭho.",
  "mil6.3.10:11.5_0": "Puna caparaṁ, mahārāja,",
  "mil6.3.10:11.5_1": "bhojanaṁ sabbasattānaṁ balavaḍḍhanaṁ;",
  "mil6.3.10:11.6_0": "evameva kho, mahārāja,",
  "mil6.3.10:11.6_1": "nibbānaṁ sacchikataṁ sabbasattānaṁ iddhibalavaḍḍhanaṁ.",
  "mil6.3.10:11.7_0": "Ayaṁ, mahārāja,",
  "mil6.3.10:11.7_1": "bhojanassa dutiyo guṇo nibbānaṁ anupaviṭṭho.",
  "mil6.3.10:11.8_0": "Puna caparaṁ, mahārāja,",
  "mil6.3.10:11.8_1": "bhojanaṁ sabbasattānaṁ vaṇṇajananaṁ;",
  "mil6.3.10:11.9_0": "evameva kho, mahārāja,",
  "mil6.3.10:11.9_1": "nibbānaṁ sacchikataṁ sabbasattānaṁ guṇavaṇṇajananaṁ.",
  "mil6.3.10:11.10_0": "Ayaṁ, mahārāja,",
  "mil6.3.10:11.10_1": "bhojanassa tatiyo guṇo nibbānaṁ anupaviṭṭho.",
  "mil6.3.10:11.11_0": "Puna caparaṁ, mahārāja,",
  "mil6.3.10:11.11_1": "bhojanaṁ sabbasattānaṁ darathavūpasamanaṁ;",
  "mil6.3.10:11.12_0": "evameva kho, mahārāja,",
  "mil6.3.10:11.12_1": "nibbānaṁ sacchikataṁ sabbasattānaṁ sabbakilesadarathavūpasamanaṁ.",
  "mil6.3.10:11.13_0": "Ayaṁ, mahārāja,",
  "mil6.3.10:11.13_1": "bhojanassa catuttho guṇo nibbānaṁ anupaviṭṭho.",
  "mil6.3.10:11.14_0": "Puna caparaṁ, mahārāja,",
  "mil6.3.10:11.14_1": "bhojanaṁ sabbasattānaṁ jighacchādubbalyapaṭivinodanaṁ;",
  "mil6.3.10:11.15_0": "evameva kho, mahārāja,",
  "mil6.3.10:11.15_1": "nibbānaṁ sacchikataṁ sabbasattānaṁ sabbadukkhajighacchādubbalyapaṭivinodanaṁ.",
  "mil6.3.10:11.16_0": "Ayaṁ, mahārāja,",
  "mil6.3.10:11.16_1": "bhojanassa pañcamo guṇo nibbānaṁ anupaviṭṭho.",
  "mil6.3.10:11.17_0": "Ime kho, mahārāja,",
  "mil6.3.10:11.17_1": "bhojanassa pañca guṇā nibbānaṁ anupaviṭṭhā”ti.",
  "mil6.3.10:12.1_0": "“Bhante nāgasena,",
  "mil6.3.10:12.1_1": "‘ākāsassa dasa guṇā nibbānaṁ anupaviṭṭhā’ti yaṁ vadesi,",
  "mil6.3.10:12.1_2": "katame ākāsassa dasa guṇā nibbānaṁ anupaviṭṭhā”ti?",
  "mil6.3.10:12.2_0": "“Yathā, mahārāja, ākāso na jāyati, na jīyati, na mīyati, na cavati, na uppajjati, duppasaho, acorāharaṇo, anissito, vihagagamano, nirāvaraṇo, ananto.",
  "mil6.3.10:12.3_0": "Evameva kho, mahārāja, nibbānaṁ na jāyati, na jīyati, na mīyati, na cavati, na uppajjati, duppasahaṁ, acorāharaṇaṁ, anissitaṁ, ariyagamanaṁ, nirāvaraṇaṁ, anantaṁ.",
  "mil6.3.10:12.4_0": "Ime kho, mahārāja,",
  "mil6.3.10:12.4_1": "ākāsassa dasa guṇā nibbānaṁ anupaviṭṭhā”ti.",
  "mil6.3.10:13.1_0": "“Bhante nāgasena,",
  "mil6.3.10:13.1_1": "‘maṇiratanassa tayo guṇā nibbānaṁ anupaviṭṭhā’ti yaṁ vadesi,",
  "mil6.3.10:13.1_2": "katame maṇiratanassa tayo guṇā nibbānaṁ anupaviṭṭhā”ti?",
  "mil6.3.10:13.2_0": "“Yathā, mahārāja, maṇiratanaṁ kāmadadaṁ;",
  "mil6.3.10:13.3_0": "evameva kho, mahārāja, nibbānaṁ kāmadadaṁ.",
  "mil6.3.10:13.4_0": "Ayaṁ, mahārāja,",
  "mil6.3.10:13.4_1": "maṇiratanassa paṭhamo guṇo nibbānaṁ anupaviṭṭho.",
  "mil6.3.10:13.5_0": "Puna caparaṁ, mahārāja, maṇiratanaṁ hāsakaraṁ;",
  "mil6.3.10:13.6_0": "evameva kho, mahārāja, nibbānaṁ hāsakaraṁ.",
  "mil6.3.10:13.7_0": "Ayaṁ, mahārāja,",
  "mil6.3.10:13.7_1": "maṇiratanassa dutiyo guṇo nibbānaṁ anupaviṭṭho.",
  "mil6.3.10:13.8_0": "Puna caparaṁ, mahārāja, maṇiratanaṁ ujjotattakaraṁ;",
  "mil6.3.10:13.9_0": "evameva kho, mahārāja, nibbānaṁ ujjotattakaraṁ.",
  "mil6.3.10:13.10_0": "Ayaṁ, mahārāja,",
  "mil6.3.10:13.10_1": "maṇiratanassa tatiyo guṇo nibbānaṁ anupaviṭṭho.",
  "mil6.3.10:13.11_0": "Ime kho, mahārāja,",
  "mil6.3.10:13.11_1": "maṇiratanassa tayo guṇā nibbānaṁ anupaviṭṭhā”ti.",
  "mil6.3.10:14.1_0": "“Bhante nāgasena,",
  "mil6.3.10:14.1_1": "‘lohitacandanassa tayo guṇā nibbānaṁ anupaviṭṭhā’ti yaṁ vadesi,",
  "mil6.3.10:14.1_2": "katame lohitacandanassa tayo guṇā nibbānaṁ anupaviṭṭhā”ti?",
  "mil6.3.10:14.2_0": "“Yathā, mahārāja, lohitacandanaṁ dullabhaṁ;",
  "mil6.3.10:14.3_0": "evameva kho, mahārāja, nibbānaṁ dullabhaṁ.",
  "mil6.3.10:14.4_0": "Ayaṁ, mahārāja,",
  "mil6.3.10:14.4_1": "lohitacandanassa paṭhamo guṇo nibbānaṁ anupaviṭṭho.",
  "mil6.3.10:14.5_0": "Puna caparaṁ, mahārāja,",
  "mil6.3.10:14.5_1": "lohitacandanaṁ asamasugandhaṁ;",
  "mil6.3.10:14.6_0": "evameva kho, mahārāja, nibbānaṁ asamasugandhaṁ.",
  "mil6.3.10:14.7_0": "Ayaṁ, mahārāja,",
  "mil6.3.10:14.7_1": "lohitacandanassa dutiyo guṇo nibbānaṁ anupaviṭṭho.",
  "mil6.3.10:14.8_0": "Puna caparaṁ, mahārāja,",
  "mil6.3.10:14.8_1": "lohitacandanaṁ sajjanapasatthaṁ;",
  "mil6.3.10:14.9_0": "evameva kho, mahārāja,",
  "mil6.3.10:14.9_1": "nibbānaṁ ariyasajjanapasatthaṁ.",
  "mil6.3.10:14.10_0": "Ayaṁ, mahārāja,",
  "mil6.3.10:14.10_1": "lohitacandanassa tatiyo guṇo nibbānaṁ anupaviṭṭho.",
  "mil6.3.10:14.11_0": "Ime kho, mahārāja,",
  "mil6.3.10:14.11_1": "lohitacandanassa tayo guṇā nibbānaṁ anupaviṭṭhā”ti.",
  "mil6.3.10:15.1_0": "“Bhante nāgasena,",
  "mil6.3.10:15.1_1": "‘sappimaṇḍassa tayo guṇā nibbānaṁ anupaviṭṭhā’ti yaṁ vadesi,",
  "mil6.3.10:15.1_2": "katame sappimaṇḍassa tayo guṇā nibbānaṁ anupaviṭṭhā”ti?",
  "mil6.3.10:15.2_0": "“Yathā, mahārāja, sappimaṇḍo vaṇṇasampanno;",
  "mil6.3.10:15.3_0": "evameva kho, mahārāja, nibbānaṁ guṇavaṇṇasampannaṁ.",
  "mil6.3.10:15.4_0": "Ayaṁ, mahārāja,",
  "mil6.3.10:15.4_1": "sappimaṇḍassa paṭhamo guṇo nibbānaṁ anupaviṭṭho.",
  "mil6.3.10:15.5_0": "Puna caparaṁ, mahārāja, sappimaṇḍo gandhasampanno;",
  "mil6.3.10:15.6_0": "evameva kho, mahārāja, nibbānaṁ sīlagandhasampannaṁ.",
  "mil6.3.10:15.7_0": "Ayaṁ, mahārāja,",
  "mil6.3.10:15.7_1": "sappimaṇḍassa dutiyo guṇo nibbānaṁ anupaviṭṭho.",
  "mil6.3.10:15.8_0": "Puna caparaṁ, mahārāja, sappimaṇḍo rasasampanno;",
  "mil6.3.10:15.9_0": "evameva kho, mahārāja, nibbānaṁ rasasampannaṁ.",
  "mil6.3.10:15.10_0": "Ayaṁ, mahārāja,",
  "mil6.3.10:15.10_1": "sappimaṇḍassa tatiyo guṇo nibbānaṁ anupaviṭṭho.",
  "mil6.3.10:15.11_0": "Ime kho, mahārāja,",
  "mil6.3.10:15.11_1": "sappimaṇḍassa tayo guṇā nibbānaṁ anupaviṭṭhā”ti.",
  "mil6.3.10:16.1_0": "“Bhante nāgasena,",
  "mil6.3.10:16.1_1": "‘girisikharassa pañca guṇā nibbānaṁ anupaviṭṭhā’ti yaṁ vadesi,",
  "mil6.3.10:16.1_2": "katame girisikharassa pañca guṇā nibbānaṁ anupaviṭṭhā”ti?",
  "mil6.3.10:16.2_0": "“Yathā, mahārāja, girisikharaṁ accuggataṁ;",
  "mil6.3.10:16.3_0": "evameva kho, mahārāja, nibbānaṁ accuggataṁ.",
  "mil6.3.10:16.4_0": "Ayaṁ, mahārāja,",
  "mil6.3.10:16.4_1": "girisikharassa paṭhamo guṇo nibbānaṁ anupaviṭṭho.",
  "mil6.3.10:16.5_0": "Puna caparaṁ, mahārāja, girisikharaṁ acalaṁ;",
  "mil6.3.10:16.6_0": "evameva kho, mahārāja, nibbānaṁ acalaṁ.",
  "mil6.3.10:16.7_0": "Ayaṁ, mahārāja,",
  "mil6.3.10:16.7_1": "girisikharassa dutiyo guṇo nibbānaṁ anupaviṭṭho.",
  "mil6.3.10:16.8_0": "Puna caparaṁ, mahārāja, girisikharaṁ duradhirohaṁ;",
  "mil6.3.10:16.9_0": "evameva kho, mahārāja,",
  "mil6.3.10:16.9_1": "nibbānaṁ duradhirohaṁ sabbakilesānaṁ.",
  "mil6.3.10:16.10_0": "Ayaṁ, mahārāja,",
  "mil6.3.10:16.10_1": "girisikharassa tatiyo guṇo nibbānaṁ anupaviṭṭho.",
  "mil6.3.10:16.11_0": "Puna caparaṁ, mahārāja,",
  "mil6.3.10:16.11_1": "girisikharaṁ sabbabījānaṁ avirūhanaṁ;",
  "mil6.3.10:16.12_0": "evameva kho, mahārāja,",
  "mil6.3.10:16.12_1": "nibbānaṁ sabbakilesānaṁ avirūhanaṁ.",
  "mil6.3.10:16.13_0": "Ayaṁ, mahārāja,",
  "mil6.3.10:16.13_1": "girisikharassa catuttho guṇo nibbānaṁ anupaviṭṭho.",
  "mil6.3.10:16.14_0": "Puna caparaṁ, mahārāja,",
  "mil6.3.10:16.14_1": "girisikharaṁ anunayappaṭighavippamuttaṁ;",
  "mil6.3.10:16.15_0": "evameva kho, mahārāja,",
  "mil6.3.10:16.15_1": "nibbānaṁ anunayappaṭighavippamuttaṁ.",
  "mil6.3.10:16.16_0": "Ayaṁ, mahārāja,",
  "mil6.3.10:16.16_1": "girisikharassa pañcamo guṇo nibbānaṁ anupaviṭṭho.",
  "mil6.3.10:16.17_0": "Ime kho, mahārāja,",
  "mil6.3.10:16.17_1": "girisikharassa pañca guṇā nibbānaṁ anupaviṭṭhā”ti.",
  "mil6.3.10:16.18_0": "“Sādhu, bhante nāgasena,",
  "mil6.3.10:16.18_1": "evametaṁ tathā sampaṭicchāmī”ti.",
  "mil6.3.10:17.1_0": "Nibbānarūpasaṇṭhānapañho dasamo."
}