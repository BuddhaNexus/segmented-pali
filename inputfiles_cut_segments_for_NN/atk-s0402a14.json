{
  "atk-s0402a14:0_0": "(15) 5. Samāpattivaggavaṇṇanā",
  "atk-s0402a14:1_0": "164.",
  "atk-s0402a14:1_1": "Pañcamassa paṭhame samāpattikusalatāti āhārasappāyaṁ utusappāyaṁ pariggaṇhitvā samāpattisamāpajjane chekatā.",
  "atk-s0402a14:1_2": "Samāpattivuṭṭhānakusalatāti yathāparicchedena gate kāle viyatto hutvā uṭṭhahanto vuṭṭhānakusalo nāma hoti,",
  "atk-s0402a14:1_3": "evaṁ kusalatā.",
  "atk-s0402a14:2_0": "165. Dutiye ajjavanti ujubhāvo. Maddavanti mudubhāvo.",
  "atk-s0402a14:3_0": "166. Tatiye khantīti adhivāsanakhanti. Soraccanti susīlyabhāvena suratabhāvo.",
  "atk-s0402a14:4_0": "167.",
  "atk-s0402a14:4_1": "Catutthe sākhalyanti saṇhavācāvasena sammodamānabhāvo.",
  "atk-s0402a14:4_2": "Paṭisanthāroti āmisena vā dhammena vā paṭisantharaṇaṁ.",
  "atk-s0402a14:5_0": "168.",
  "atk-s0402a14:5_1": "Pañcame avihiṁsāti karuṇāpubbabhāgo.",
  "atk-s0402a14:5_2": "Soceyyanti sīlavasena sucibhāvo.",
  "atk-s0402a14:5_3": "Chaṭṭhasattamāni uttānatthāneva.",
  "atk-s0402a14:6_0": "171. Aṭṭhame paṭisaṅkhānabalanti paccavekkhaṇabalaṁ.",
  "atk-s0402a14:7_0": "172.",
  "atk-s0402a14:7_1": "Navame muṭṭhassacce akampanena satiyeva satibalaṁ.",
  "atk-s0402a14:7_2": "Uddhacce akampanena samādhiyeva samādhibalaṁ.",
  "atk-s0402a14:8_0": "173. Dasame samathoti cittekaggatā. Vipassanāti saṅkhārapariggāhakaññāṇaṁ.",
  "atk-s0402a14:9_0": "174. Ekādasame sīlavipattīti dussīlyaṁ. Diṭṭhivipattīti micchādiṭṭhi.",
  "atk-s0402a14:10_0": "175.",
  "atk-s0402a14:10_1": "Dvādasame sīlasampadāti paripuṇṇasīlatā.",
  "atk-s0402a14:10_2": "Diṭṭhisampadāti sammādiṭṭhikabhāvo.",
  "atk-s0402a14:10_3": "Tena kammassakatasammādiṭṭhi,",
  "atk-s0402a14:10_4": "jhānasammādiṭṭhi,",
  "atk-s0402a14:10_5": "vipassanāsammādiṭṭhi,",
  "atk-s0402a14:10_6": "maggasammādiṭṭhi,",
  "atk-s0402a14:10_7": "phalasammādiṭṭhīti sabbāpi pañcavidhā sammādiṭṭhi saṅgahitā hoti.",
  "atk-s0402a14:11_0": "176.",
  "atk-s0402a14:11_1": "Terasame sīlavisuddhīti visuddhisampāpakaṁ sīlaṁ.",
  "atk-s0402a14:11_2": "Diṭṭhivisuddhīti visuddhisampāpikā catumaggasammādiṭṭhi,",
  "atk-s0402a14:11_3": "pañcavidhāpi vā sammādiṭṭhi.",
  "atk-s0402a14:12_0": "177.",
  "atk-s0402a14:12_1": "Cuddasame diṭṭhivisuddhīti visuddhisampāpikā sammādiṭṭhiyeva.",
  "atk-s0402a14:12_2": "Yathādiṭṭhissa ca padhānanti heṭṭhimamaggasampayuttaṁ vīriyaṁ.",
  "atk-s0402a14:12_3": "Tañhi tassā diṭṭhiyā anurūpattā “yathādiṭṭhissa ca padhāna”nti vuttaṁ.",
  "atk-s0402a14:13_0": "178.",
  "atk-s0402a14:13_1": "Pannarasame asantuṭṭhitā ca kusalesu dhammesūti aññatra arahattamaggā kusalesu dhammesu asantuṭṭhibhāvo.",
  "atk-s0402a14:14_0": "179. Soḷasame muṭṭhassaccanti muṭṭhassatibhāvo. Asampajaññanti aññāṇabhāvo.",
  "atk-s0402a14:15_0": "180. Sattarasame apilāpanalakkhaṇā sati. Sammā pajānanalakkhaṇaṁ sampajaññanti.",
  "atk-s0402a14:16_0": "Samāpattivaggo pañcamo. Tatiyapaṇṇāsakaṁ niṭṭhitaṁ."
}