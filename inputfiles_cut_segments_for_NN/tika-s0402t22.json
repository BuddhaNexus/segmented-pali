{
  "tika-s0402t22:0_0": "5. Cūḷavaggo",
  "tika-s0402t22:1_0": "1. Sammukhībhāvasuttavaṇṇanā",
  "tika-s0402t22:2_0": "41.",
  "tika-s0402t22:2_1": "Pañcamassa paṭhame sammukho bhavati yena so sammukhībhāvo,",
  "tika-s0402t22:2_2": "purato vijjamānatā,",
  "tika-s0402t22:2_3": "tasmā sammukhībhāvā.",
  "tika-s0402t22:2_4": "Puññakammanti dānasaṅkhātaṁ puññakammaṁ.",
  "tika-s0402t22:2_5": "Dve dhammā sulabhā bāhirattā yathāsakaṁ paccayasamavāyena labbhanato.",
  "tika-s0402t22:2_6": "Saddhā pana dullabhā pacurajanassa anavaṭṭhitakiccattā.",
  "tika-s0402t22:2_7": "Tenevāha “puthujjanassā”tiādi.",
  "tika-s0402t22:3_0": "Sammukhībhāvasuttavaṇṇanā niṭṭhitā.",
  "tika-s0402t22:4_0": "2. Tiṭhānasuttavaṇṇanā",
  "tika-s0402t22:5_0": "42.",
  "tika-s0402t22:5_1": "Dutiye macchariyameva malaṁ macchariyamalaṁ,",
  "tika-s0402t22:5_2": "cittassa malīnasabhāvāpādanato vigataṁ macchariyamalaṁ etthāti vigatamacchariyamalaṁ.",
  "tika-s0402t22:5_3": "Gedhābhāvena koci kiñci dentopi tattha āsattiṁ na vissajjeti,",
  "tika-s0402t22:5_4": "ayaṁ pana na tādisoti āha “vissaṭṭhacāgo”ti.",
  "tika-s0402t22:5_5": "Malīnahatthova cittavisuddhiyā abhāvato.",
  "tika-s0402t22:5_6": "Dhotahatthova dhotahatthena kātabbakiccasādhanato.",
  "tika-s0402t22:5_7": "Tena vuttaṁ –",
  "tika-s0402t22:5_8": "“citte suddhe visujjhanti,",
  "tika-s0402t22:5_9": "iti vuttaṁ mahesinā”ti.",
  "tika-s0402t22:5_10": "Yācituṁ yutto yācakānaṁ manorathapūraṇato.",
  "tika-s0402t22:5_11": "Yācayogo payogāsahehi yācakehi suṭṭhu yuttabhāvato.",
  "tika-s0402t22:5_12": "Taṁsamaṅgī eva tattha rato nāma,",
  "tika-s0402t22:5_13": "na cittamattenevāti āha “dānaṁ…pe…",
  "tika-s0402t22:5_14": "rato nāma hotī”ti.",
  "tika-s0402t22:6_0": "“Uccā,",
  "tika-s0402t22:6_1": "bhante”ti vatvā ayaṁ maṁ uccato vadatīti cinteyyāti puna “nātiuccā tumhe”ti āha,",
  "tika-s0402t22:6_2": "sarīrenāti adhippāyo.",
  "tika-s0402t22:6_3": "Mecakavaṇṇassāti nīlobhāsassa.",
  "tika-s0402t22:6_4": "Pucchīti bhikkhū pucchi.",
  "tika-s0402t22:6_5": "Bhūmiyaṁ lekhaṁ likhanto acchīti paṭhamaṁ mañce nipajjitvā uṭṭhāya bhūmiyaṁ lekhaṁ likhanto acchi “akhīṇāsavoti maññanā hotū”ti.",
  "tika-s0402t22:6_6": "Tathā hi rājā khīṇāsavassa nāma…pe… nivatti.",
  "tika-s0402t22:6_7": "Dhajapaggahitāvāti paggahitadhajāva.",
  "tika-s0402t22:6_8": "Silācetiyaṭṭhānanti thūpārāmassa ca mahācetiyassa ca antare silāya katacetiyaṭṭhānaṁ.",
  "tika-s0402t22:6_9": "Cetiyaṁ…pe…",
  "tika-s0402t22:6_10": "aṭṭhāsi sāvakassa taṁ kūṭāgāranti katvā.",
  "tika-s0402t22:7_0": "Pacchābhāgenāti dhammakathikassa therassa piṭṭhipassena.",
  "tika-s0402t22:7_1": "Gonasoti maṇḍalasappo.",
  "tika-s0402t22:7_2": "Dhammassavanantarāya bahūnaṁ saggamaggappaṭilābhantarāyo bhaveyyāti adhippāyena “dhammassavanantarāyaṁ na karissāmī”ti cintesi.",
  "tika-s0402t22:7_3": "Visaṁ vikkhambhetvāti vipassanātejena visavegaṁ vikkhambhetvā.",
  "tika-s0402t22:7_4": "Gāthāsahassanti gāthāsahassavantaṁ.",
  "tika-s0402t22:7_5": "Paṭṭhānagāthāyāti paṭṭhāpanagāthāya,",
  "tika-s0402t22:7_6": "ādigāthāyāti attho.",
  "tika-s0402t22:7_7": "Paṭṭhāna…pe…",
  "tika-s0402t22:7_8": "avasānagāthaṁ eva vavatthapesi,",
  "tika-s0402t22:7_9": "na dvinnaṁ antare vuttaṁ kilantakāyattā.",
  "tika-s0402t22:7_10": "Sarabhāṇaṁ sāyanhadhammakathā.",
  "tika-s0402t22:7_11": "Paggaṇhātīti paccakkhaṁ karontī gaṇhāti,",
  "tika-s0402t22:7_12": "sakkaccaṁ suṇātīti attho.",
  "tika-s0402t22:7_13": "Dhammakathanadivase dhammakathikānaṁ akilamanatthaṁ saddhā upāsakā siniddhabhojanaṁ madhupānakañca denti",
  "tika-s0402t22:7_14": "sarassa madhurabhāvāya sappimadhukatelādiñca bhesajjaṁ.",
  "tika-s0402t22:7_15": "Tenāha “ariyavaṁsaṁ kathessāmī”tiādi.",
  "tika-s0402t22:7_16": "Catūhi dāṭhāhi ḍaṁsitvāti daḷhadaṭṭhabhāvadassanaṁ.",
  "tika-s0402t22:7_17": "Carissāmīti sammānessāmi,",
  "tika-s0402t22:7_18": "sakkaccaṁ suṇissāmīti attho.",
  "tika-s0402t22:7_19": "Nimmathetvāti nimmadditvā,",
  "tika-s0402t22:7_20": "apanetvāti attho.",
  "tika-s0402t22:8_0": "Tiṭhānasuttavaṇṇanā niṭṭhitā.",
  "tika-s0402t22:9_0": "3. Atthavasasuttavaṇṇanā",
  "tika-s0402t22:10_0": "43.",
  "tika-s0402t22:10_1": "Tatiye attho nāma phalaṁ,",
  "tika-s0402t22:10_2": "taṁ etassa vasoti atthavaso. Hetu,",
  "tika-s0402t22:10_3": "attho etassa atthīti attho,",
  "tika-s0402t22:10_4": "so evāti āha “tayo atthe tīṇi kāraṇānī”ti.",
  "tika-s0402t22:10_5": "Dhammadesanā nāma ukkaṭṭhaniddesena catunnaṁ ariyasaccānaṁ pakāsanāti āha “catusaccadhammaṁ pakāsetī”ti.",
  "tika-s0402t22:10_6": "Aṭṭhakathaṁ ñāṇena paṭisaṁvedīti pāḷipadānaṁ atthaṁ vivaraṇañāṇena paṭi paṭi saṁvedanasīlo “ayaṁ imassa bhāsitassa attho”ti.",
  "tika-s0402t22:10_7": "Etena atthapaṭisambhidābyāpāramāha.",
  "tika-s0402t22:10_8": "Pāḷidhammaṁ paṭisaṁvedīti pāḷigatiṁ pāḷiṁ padavivaraṇaṁ paṭi paṭi saṁvedanasīlo.",
  "tika-s0402t22:10_9": "Etena dhammapaṭisambhidābyāpāramāha.",
  "tika-s0402t22:11_0": "Atthavasasuttavaṇṇanā niṭṭhitā.",
  "tika-s0402t22:12_0": "4. Kathāpavattisuttavaṇṇanā",
  "tika-s0402t22:13_0": "44.",
  "tika-s0402t22:13_1": "Catutthe pavattinīti yāva adhippetatthanigamanā avicchedena pavattinī.",
  "tika-s0402t22:13_2": "Paṭighātābhāvena appaṭihatā.",
  "tika-s0402t22:13_3": "Niyyānikā sappāṭihīrakā.",
  "tika-s0402t22:14_0": "Kathāpavattisuttavaṇṇanā niṭṭhitā.",
  "tika-s0402t22:15_0": "5. Paṇḍitasuttavaṇṇanā",
  "tika-s0402t22:16_0": "45.",
  "tika-s0402t22:16_1": "Pañcame paṇḍitapaññattānīti paṇḍitehi paṭhamaṁ paññattāni.",
  "tika-s0402t22:16_2": "Kathitānīti seyyaso kathitāni.",
  "tika-s0402t22:16_3": "Mahāpurisehīti buddhabodhisattehi.",
  "tika-s0402t22:16_4": "Karuṇāti karuṇācetovimutti vuttā.",
  "tika-s0402t22:16_5": "Pubbabhāgoti tassa upacāro.",
  "tika-s0402t22:16_6": "Damoti indriyasaṁvaro “manacchaṭṭhānaṁ indriyānaṁ damana”nti katvā.",
  "tika-s0402t22:16_7": "Attadamananti cittadamanaṁ.",
  "tika-s0402t22:16_8": "Puṇṇovāde “sakkhissasi kho,",
  "tika-s0402t22:16_9": "tvaṁ puṇṇa,",
  "tika-s0402t22:16_10": "iminā damūpasamenā”ti āgatattā damoti vuttā khantipi.",
  "tika-s0402t22:16_11": "Āḷavake āḷavakasutte “saccā damā cāgā”ti evaṁ vuttā paññāpi imasmiṁ sutte “damo”ti vattuṁ vaṭṭati.",
  "tika-s0402t22:16_12": "Rakkhanaṁ gopanaṁ paṭijaggananti mātāpitūnaṁ manussāmanussakatūpaddavato rakkhanaṁ,",
  "tika-s0402t22:16_13": "byādhiādianatthato gopanaṁ,",
  "tika-s0402t22:16_14": "ghāsacchādanādīhi veyyāvaccakaraṇena paṭijagganaṁ.",
  "tika-s0402t22:16_15": "Santo nāma sabbakilesadarathapariḷāhūpasamena upasantakāyavacīsamācāratāya ca.",
  "tika-s0402t22:16_16": "Uttamaṭṭhena santānanti matteyyatādīhi seṭṭhaṭṭhena santānaṁ.",
  "tika-s0402t22:17_0": "Idha imesaṁyeva tiṇṇaṁ ṭhānānaṁ karaṇenāti imasmiṁ sutte āgatānaṁ tiṇṇaṁ ṭhānānaṁ karaṇena nibbattanena. Etāni…pe…",
  "tika-s0402t22:17_1": "kāraṇānīti mātupaṭṭhānaṁ pitupaṭṭhānanti etāni dve uttamapurisānaṁ kāraṇāni.",
  "tika-s0402t22:17_2": "Uttamakiccakaraṇena hi mātāpituupaṭṭhākā “uttamapurisā”ti vuttā.",
  "tika-s0402t22:17_3": "Tenāha “mātāpitu…pe… vutto”ti.",
  "tika-s0402t22:17_4": "Anupaddavabhāvena khemaṁ.",
  "tika-s0402t22:18_0": "Paṇḍitasuttavaṇṇanā niṭṭhitā.",
  "tika-s0402t22:19_0": "6. Sīlavantasuttavaṇṇanā",
  "tika-s0402t22:20_0": "46.",
  "tika-s0402t22:20_1": "Chaṭṭhe lokiyalokuttaramissakanti ettha “manussā puññaṁ pasavantī”ti avisesena vuttattā bhāvanāmayassapi",
  "tika-s0402t22:20_2": "puññassa saṅgaṇhanato lokuttarassapi sambhavo daṭṭhabbo.",
  "tika-s0402t22:21_0": "Sīlavantasuttavaṇṇanā niṭṭhitā.",
  "tika-s0402t22:22_0": "7. Saṅkhatalakkhaṇasuttavaṇṇanā",
  "tika-s0402t22:23_0": "47.",
  "tika-s0402t22:23_1": "Sattame samecca sambhūya paccayehi kataṁ saṅkhataṁ.",
  "tika-s0402t22:23_2": "Nimittānīti sañjānanassa nimittāni.",
  "tika-s0402t22:23_3": "Hetupaccayasamavāye uppajjanaṁ uppādo,",
  "tika-s0402t22:23_4": "attalābho.",
  "tika-s0402t22:23_5": "Vayoti bhaṅgo.",
  "tika-s0402t22:23_6": "Ṭhitassāti uppādakkhaṇato uddhaṁ ṭhitikkhaṇapattassa.",
  "tika-s0402t22:23_7": "Sā panassa avatthā uppādāvatthāya bhinnāti katvā aññathattaṁ jarāti ca vuttā.",
  "tika-s0402t22:23_8": "Yasmā dhammo uppajjamāno eva bhijjati,",
  "tika-s0402t22:23_9": "tathā sati uppādabhaṅgā samānakkhaṇā siyuṁ,",
  "tika-s0402t22:23_10": "na ca taṁ yujjati,",
  "tika-s0402t22:23_11": "tasmā uppādāvatthāya bhinnā bhaṅgābhimukhāvatthā jarāti veditabbā.",
  "tika-s0402t22:23_12": "Ye pana “saṅkhārānaṁ ṭhiti natthī”ti vadanti,",
  "tika-s0402t22:23_13": "tesaṁ taṁ micchā.",
  "tika-s0402t22:23_14": "Yathā hi tasseva dhammassa uppādāvatthāya bhinnā bhaṅgāvatthā icchitā,",
  "tika-s0402t22:23_15": "aññathā “aññaṁ uppajjati,",
  "tika-s0402t22:23_16": "aññaṁ nirujjhatī”ti āpajjati,",
  "tika-s0402t22:23_17": "evaṁ uppajjamānassa bhaṅgābhimukhā dhammā icchitabbā.",
  "tika-s0402t22:23_18": "Sā ca ṭhitikkhaṇo.",
  "tika-s0402t22:23_19": "Na hi uppajjamāno bhijjatīti sakkā viññātunti.",
  "tika-s0402t22:24_0": "Saṅkhatanti tebhūmakā dhammā paccayasamuppannattā.",
  "tika-s0402t22:24_1": "Yadi evaṁ maggaphaladhammā kathanti āha “maggaphalāni panā”tiādi.",
  "tika-s0402t22:24_2": "Lakkhaṇakathā hi yāvadeva sammasanatthā.",
  "tika-s0402t22:24_3": "Uppādakkhaṇe uppādo,",
  "tika-s0402t22:24_4": "na ṭhānabhaṅgakkhaṇesu. Kasmā?",
  "tika-s0402t22:24_5": "Uppādauppādakkhaṇānaṁ aññamaññaṁ paricchinnattā.",
  "tika-s0402t22:24_6": "Yathā hi uppādasaṅkhātena vikārena uppādakkhaṇo paricchinno,",
  "tika-s0402t22:24_7": "evaṁ uppādakkhaṇenapi uppādo paricchinno.",
  "tika-s0402t22:24_8": "Sesadvayepi eseva nayo.",
  "tika-s0402t22:24_9": "Dhammappavattimattatāyapi kālassa lokasamaññāvaseneva vuttaṁ.",
  "tika-s0402t22:24_10": "Lakkhaṇaṁ na saṅkhataṁ,",
  "tika-s0402t22:24_11": "saṅkhataṁ na lakkhaṇanti nesaṁ bhedadassanaṁ.",
  "tika-s0402t22:24_12": "Avatthāvato hi avatthā bhinnāvāti.",
  "tika-s0402t22:24_13": "Paricchinnanti ettha uppādavayehi tāva saṅkhataṁ paricchinnaṁ hotu,",
  "tika-s0402t22:24_14": "jarāya pana taṁ kathaṁ paricchinnanti vuccati?",
  "tika-s0402t22:24_15": "Na vuccati paricchedo pubbantāparantamattena,",
  "tika-s0402t22:24_16": "atha kho sabhāvabhedenāti nāyaṁ doso.",
  "tika-s0402t22:24_17": "Saṅkhataṁ dhammajātaṁ paricchinnaṁ tabbantaṁ dhammajātaṁ saṅkhatanti paññāyati evaṁ tesaṁ abhāvena nibbānametanti lakkhitabbato sañjānitabbato.",
  "tika-s0402t22:24_18": "Idāni “yathā hī”tiādinā yathāvuttamattaṁ upamāhi vibhāveti.",
  "tika-s0402t22:25_0": "Saṅkhatalakkhaṇasuttavaṇṇanā niṭṭhitā.",
  "tika-s0402t22:26_0": "8. Asaṅkhatalakkhaṇasuttavaṇṇanā",
  "tika-s0402t22:27_0": "48.",
  "tika-s0402t22:27_1": "Aṭṭhame asaṅkhatassāti vuttanayena na saṅkhatassa.",
  "tika-s0402t22:27_2": "Tenāha “paccayehī”tiādi.",
  "tika-s0402t22:27_3": "Yathā uppādādīnaṁ bhāvena saṅkhatadhammajātaṁ saṅkhatanti paññāyati,",
  "tika-s0402t22:27_4": "evaṁ tesaṁ abhāvena nibbānaṁ asaṅkhatanti paññāyati.",
  "tika-s0402t22:28_0": "Asaṅkhatalakkhaṇasuttavaṇṇanā niṭṭhitā.",
  "tika-s0402t22:29_0": "9. Pabbatarājasuttavaṇṇanā",
  "tika-s0402t22:30_0": "49.",
  "tika-s0402t22:30_1": "Navame idha sāla-saddo rukkhasāmaññapariyāyo,",
  "tika-s0402t22:30_2": "na rukkhavisesapariyāyoti āha “mahāsālāti mahārukkhā”ti.",
  "tika-s0402t22:30_3": "Kulajeṭṭhakanti tasmiṁ kule jeṭṭhabhūtaṁ sāmibhūtaṁ.",
  "tika-s0402t22:30_4": "Silāmayo na paṁsumayo missako ca.",
  "tika-s0402t22:30_5": "Gāmaṁ gāmūpacārañca ṭhapetvā sabbaṁ araññanti āha “araññasminti agāmakaṭṭhāne”ti.",
  "tika-s0402t22:30_6": "Mahanto pabbato seloti yojanā.",
  "tika-s0402t22:31_0": "Pabbatarājasuttavaṇṇanā niṭṭhitā.",
  "tika-s0402t22:32_0": "10. Ātappakaraṇīyasuttavaṇṇanā",
  "tika-s0402t22:33_0": "50.",
  "tika-s0402t22:33_1": "Dasame sarīrasambhavānanti sarīre sambhūtānaṁ.",
  "tika-s0402t22:33_2": "Dukkhānanti aniṭṭhānaṁ.",
  "tika-s0402t22:33_3": "Bahalānanti nirantarappavattiyā aviraḷānaṁ.",
  "tika-s0402t22:33_4": "Tāpanavasenāti dukkhāpanavasena.",
  "tika-s0402t22:33_5": "Tibbānanti kurūrānaṁ.",
  "tika-s0402t22:33_6": "Tāsaṁ yathāvuttānaṁ adhivāsanāya pahātabbadukkhavedanānaṁ pajahanaṁ nāma khamanamevāti āha “khamanatthāyā”ti.",
  "tika-s0402t22:33_7": "Āṇāpetvāti “ātappaṁ karaṇīya”nti buddhāṇaṁ vidhāya.",
  "tika-s0402t22:33_8": "Lokiyalokuttaramissakā kathitā sasambhārānaṁ maggadhammānaṁ kathitattā.",
  "tika-s0402t22:34_0": "Ātappakaraṇīyasuttavaṇṇanā niṭṭhitā.",
  "tika-s0402t22:35_0": "11. Mahācorasuttavaṇṇanā",
  "tika-s0402t22:36_0": "51.",
  "tika-s0402t22:36_1": "Ekādasame mahābalavacoro mahācoroti āha “mahanto balavacoro”ti.",
  "tika-s0402t22:36_2": "Balavacoroti ca mahāthāmatāya mahāparivāratāya mahācoriyakammasamatthatāya ca veditabbo.",
  "tika-s0402t22:36_3": "Mahataṁ gāmanigamānaṁ viluppanaṁ mahāvilopo.",
  "tika-s0402t22:36_4": "Taṁ taṁ kāraṇaṁ pakkhipitvāti taṁ taṁ akaraṇameva kāraṇaṁ katvā tappaṭibaddhāya kathāya pakkhipitvā.",
  "tika-s0402t22:36_5": "Atthaṁ kathayissantīti tassa tassa atthañca kathayissanti.",
  "tika-s0402t22:36_6": "Harantāti apanentā pariharantā.",
  "tika-s0402t22:36_7": "Dasavatthukāyāti “sassato loko”ti ādidasavatthusannissitāya.",
  "tika-s0402t22:36_8": "Antaṁ gahetvā ṭhitadiṭṭhiyāti tameva sassatādiantaṁ gahetvā avissajjetvā ṭhitadiṭṭhiyā.",
  "tika-s0402t22:37_0": "Mahācorasuttavaṇṇanā niṭṭhitā.",
  "tika-s0402t22:38_0": "Cūḷavaggavaṇṇanā niṭṭhitā.",
  "tika-s0402t22:39_0": "Paṭhamapaṇṇāsakaṁ niṭṭhitaṁ.",
  "tika-s0402t22:40_0": "2. Dutiyapaṇṇāsakaṁ"
}