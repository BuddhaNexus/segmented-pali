{
  "ps2.5:0.1_0": "Paṭisambhidāmagga",
  "ps2.5:0.2_0": "2 Yuganaddhavagga",
  "ps2.5:0.3_0": "2.5. Virāgakathā",
  "ps2.5:1.1_0": "Virāgo maggo, vimutti phalaṁ.",
  "ps2.5:1.2_0": "Kathaṁ **virāgo maggo?**",
  "ps2.5:1.3_0": "Sotāpattimaggakkhaṇe dassanaṭṭhena sammādiṭṭhi micchādiṭṭhiyā virajjati,",
  "ps2.5:1.3_1": "tadanuvattakakilesehi ca khandhehi ca virajjati,",
  "ps2.5:1.3_2": "bahiddhā ca sabbanimittehi virajjati.",
  "ps2.5:1.4_0": "Virāgo virāgārammaṇo virāgagocaro virāge samudāgato virāge ṭhito virāge patiṭṭhito.",
  "ps2.5:2.1_0": "**Virāgo**ti dve virāgā—",
  "ps2.5:2.2_0": "nibbānañca virāgo,",
  "ps2.5:2.2_1": "ye ca nibbānārammaṇatājātā dhammā sabbe virāgā hontīti—virāgā.",
  "ps2.5:2.3_0": "Sahajātāni sattaṅgāni virāgaṁ gacchantīti—virāgo maggo.",
  "ps2.5:2.4_0": "Etena maggena buddhā ca sāvakā ca agataṁ disaṁ nibbānaṁ gacchantīti—",
  "ps2.5:2.5_0": "aṭṭhaṅgiko maggo.",
  "ps2.5:2.6_0": "Yāvatā puthusamaṇabrāhmaṇānaṁ parappavādānaṁ maggā,",
  "ps2.5:2.6_1": "ayameva ariyo aṭṭhaṅgiko maggo aggo ca seṭṭho ca pāmokkho ca uttamo ca pavaro cāti—",
  "ps2.5:2.7_0": "maggānaṁ aṭṭhaṅgiko seṭṭho.",
  "ps2.5:3.1_0": "Abhiniropanaṭṭhena sammāsaṅkappo micchāsaṅkappā virajjati.",
  "ps2.5:3.2_0": "Pariggahaṭṭhena sammāvācā micchāvācāya virajjati.",
  "ps2.5:3.3_0": "Samuṭṭhānaṭṭhena sammākammanto micchākammantā virajjati.",
  "ps2.5:3.4_0": "Vodānaṭṭhena sammāājīvo micchāājīvā virajjati.",
  "ps2.5:3.5_0": "Paggahaṭṭhena sammāvāyāmo micchāvāyāmā virajjati.",
  "ps2.5:3.6_0": "Upaṭṭhānaṭṭhena sammāsati micchāsatiyā virajjati.",
  "ps2.5:3.7_0": "Avikkhepaṭṭhena sammāsamādhi micchāsamādhito virajjati.",
  "ps2.5:3.8_0": "Tadanuvattakakilesehi ca khandhehi ca virajjati.",
  "ps2.5:3.9_0": "Bahiddhā ca sabbanimittehi virajjati.",
  "ps2.5:3.10_0": "Virāgo virāgārammaṇo virāgagocaro virāge samudāgato virāge ṭhito virāge patiṭṭhito.",
  "ps2.5:4.1_0": "**Virāgo**ti dve virāgā—",
  "ps2.5:4.2_0": "nibbānañca virāgo,",
  "ps2.5:4.2_1": "ye ca nibbānārammaṇatājātā dhammā sabbe virāgā hontīti—virāgā.",
  "ps2.5:4.3_0": "Sahajātāni sattaṅgāni virāgaṁ gacchantīti—virāgo maggo.",
  "ps2.5:4.4_0": "Etena maggena buddhā ca sāvakā ca agataṁ disaṁ nibbānaṁ gacchantīti—",
  "ps2.5:4.5_0": "aṭṭhaṅgiko maggo.",
  "ps2.5:4.6_0": "Yāvatā puthusamaṇabrāhmaṇānaṁ parappavādānaṁ maggā,",
  "ps2.5:4.6_1": "ayameva ariyo aṭṭhaṅgiko maggo aggo ca seṭṭho ca pāmokkho ca uttamo ca pavaro cāti—",
  "ps2.5:4.7_0": "maggānaṁ aṭṭhaṅgiko seṭṭho.",
  "ps2.5:5.1_0": "Sakadāgāmimaggakkhaṇe dassanaṭṭhena sammādiṭṭhi …pe…",
  "ps2.5:5.2_0": "avikkhepaṭṭhena sammāsamādhi oḷārikā kāmarāgasaññojanā paṭighasaññojanā oḷārikā kāmarāgānusayā paṭighānusayā virajjati,",
  "ps2.5:5.2_1": "tadanuvattakakilesehi ca khandhehi ca virajjati,",
  "ps2.5:5.2_2": "bahiddhā ca sabbanimittehi virajjati.",
  "ps2.5:5.3_0": "Virāgo virāgārammaṇo virāgagocaro virāge samudāgato virāge ṭhito virāge patiṭṭhito.",
  "ps2.5:6.1_0": "**Virāgo**ti dve virāgā—",
  "ps2.5:6.2_0": "nibbānañca virāgo,",
  "ps2.5:6.2_1": "ye ca nibbānārammaṇatājātā dhammā sabbe virāgā hontīti—virāgā.",
  "ps2.5:6.3_0": "Sahajātāni sattaṅgāni virāgaṁ gacchantīti—virāgo maggo.",
  "ps2.5:6.4_0": "Etena maggena buddhā ca sāvakā ca agataṁ disaṁ nibbānaṁ gacchantīti—",
  "ps2.5:6.5_0": "aṭṭhaṅgiko maggo.",
  "ps2.5:6.6_0": "Yāvatā puthusamaṇabrāhmaṇānaṁ parappavādānaṁ maggā.",
  "ps2.5:6.7_0": "Ayameva ariyo aṭṭhaṅgiko maggo aggo ca seṭṭho ca pāmokkho ca uttamo ca pavaro cāti—",
  "ps2.5:6.8_0": "maggānaṁ aṭṭhaṅgiko seṭṭho.",
  "ps2.5:7.1_0": "Anāgāmimaggakkhaṇe dassanaṭṭhena sammādiṭṭhi …pe…",
  "ps2.5:7.2_0": "avikkhepaṭṭhena sammāsamādhi anusahagatā kāmarāgasaññojanā paṭighasaññojanā anusahagatā kāmarāgānusayā paṭighānusayā virajjati,",
  "ps2.5:7.2_1": "tadanuvattakakilesehi ca khandhehi ca virajjati,",
  "ps2.5:7.2_2": "bahiddhā ca sabbanimittehi virajjati.",
  "ps2.5:7.3_0": "Virāgo virāgārammaṇo …pe…",
  "ps2.5:7.4_0": "maggānaṁ aṭṭhaṅgiko seṭṭho.",
  "ps2.5:8.1_0": "Arahattamaggakkhaṇe dassanaṭṭhena sammādiṭṭhi …pe…",
  "ps2.5:8.2_0": "avikkhepaṭṭhena sammāsamādhi rūparāgā arūparāgā mānā uddhaccā avijjāya mānānusayā bhavarāgānusayā avijjānusayā virajjati,",
  "ps2.5:8.2_1": "tadanuvattakakilesehi ca khandhehi ca virajjati,",
  "ps2.5:8.2_2": "bahiddhā ca sabbanimittehi virajjati virāgo virāgārammaṇo virāgagocaro virāge samudāgato virāge ṭhito virāge patiṭṭhito.",
  "ps2.5:9.1_0": "**Virāgo**ti dve virāgā—",
  "ps2.5:9.2_0": "nibbānañca virāgo,",
  "ps2.5:9.2_1": "ye ca nibbānārammaṇatājātā dhammā sabbe virāgā hontīti—virāgā.",
  "ps2.5:9.3_0": "Sahajātāni sattaṅgāni virāgaṁ gacchantīti—virāgo maggo.",
  "ps2.5:9.4_0": "Etena maggena buddhā ca sāvakā ca agataṁ disaṁ nibbānaṁ gacchantīti—",
  "ps2.5:9.5_0": "aṭṭhaṅgiko maggo.",
  "ps2.5:9.6_0": "Yāvatā puthusamaṇabrāhmaṇānaṁ parappavādānaṁ maggā.",
  "ps2.5:9.7_0": "Ayameva ariyo aṭṭhaṅgiko maggo aggo ca seṭṭho ca pāmokkho ca uttamo ca pavaro cāti—",
  "ps2.5:9.8_0": "maggānaṁ aṭṭhaṅgiko seṭṭho.",
  "ps2.5:10.1_0": "Dassanavirāgo sammādiṭṭhi.",
  "ps2.5:10.2_0": "Abhiniropanavirāgo sammāsaṅkappo.",
  "ps2.5:10.3_0": "Pariggahavirāgo sammāvācā.",
  "ps2.5:10.4_0": "Samuṭṭhānavirāgo sammākammanto.",
  "ps2.5:10.5_0": "Vodānavirāgo sammāājīvo.",
  "ps2.5:10.6_0": "Paggahavirāgo sammāvāyāmo.",
  "ps2.5:10.7_0": "Upaṭṭhānavirāgo sammāsati.",
  "ps2.5:10.8_0": "Avikkhepavirāgo sammāsamādhi.",
  "ps2.5:10.9_0": "Upaṭṭhānavirāgo satisambojjhaṅgo.",
  "ps2.5:10.10_0": "Pavicayavirāgo dhammavicayasambojjhaṅgo.",
  "ps2.5:10.11_0": "Paggahavirāgo vīriyasambojjhaṅgo.",
  "ps2.5:10.12_0": "Pharaṇavirāgo pītisambojjhaṅgo.",
  "ps2.5:10.13_0": "Upasamavirāgo passaddhisambojjhaṅgo.",
  "ps2.5:10.14_0": "Avikkhepavirāgo samādhisambojjhaṅgo.",
  "ps2.5:10.15_0": "Paṭisaṅkhānavirāgo upekkhāsambojjhaṅgo.",
  "ps2.5:10.16_0": "Assaddhiye akampiyavirāgo saddhābalaṁ.",
  "ps2.5:10.17_0": "Kosajje akampiyavirāgo vīriyabalaṁ.",
  "ps2.5:10.18_0": "Pamāde akampiyavirāgo satibalaṁ.",
  "ps2.5:10.19_0": "Uddhacce akampiyavirāgo samādhibalaṁ.",
  "ps2.5:10.20_0": "Avijjāya akampiyavirāgo paññābalaṁ.",
  "ps2.5:10.21_0": "Adhimokkhavirāgo saddhindriyaṁ.",
  "ps2.5:10.22_0": "Paggahavirāgo vīriyindriyaṁ.",
  "ps2.5:10.23_0": "Upaṭṭhānavirāgo satindriyaṁ.",
  "ps2.5:10.24_0": "Avikkhepavirāgo samādhindriyaṁ.",
  "ps2.5:10.25_0": "Dassanavirāgo paññindriyaṁ.",
  "ps2.5:10.26_0": "Ādhipateyyaṭṭhena indriyāni virāgo.",
  "ps2.5:10.27_0": "Akampiyaṭṭhena balaṁ virāgo.",
  "ps2.5:10.28_0": "Niyyānaṭṭhena bojjhaṅgā virāgo.",
  "ps2.5:10.29_0": "Hetuṭṭhena maggo virāgo.",
  "ps2.5:10.30_0": "Upaṭṭhānaṭṭhena satipaṭṭhānā virāgo.",
  "ps2.5:10.31_0": "Padahanaṭṭhena sammappadhānā virāgo.",
  "ps2.5:10.32_0": "Ijjhanaṭṭhena iddhipādā virāgo.",
  "ps2.5:10.33_0": "Tathaṭṭhena saccā virāgo.",
  "ps2.5:10.34_0": "Avikkhepaṭṭhena samatho virāgo.",
  "ps2.5:10.35_0": "Anupassanaṭṭhena vipassanā virāgo.",
  "ps2.5:10.36_0": "Ekarasaṭṭhena samathavipassanā virāgo.",
  "ps2.5:10.37_0": "Anativattanaṭṭhena yuganaddhaṁ virāgo.",
  "ps2.5:10.38_0": "Saṁvaraṭṭhena sīlavisuddhi virāgo.",
  "ps2.5:10.39_0": "Avikkhepaṭṭhena cittavisuddhi virāgo.",
  "ps2.5:10.40_0": "Dassanaṭṭhena diṭṭhivisuddhi virāgo.",
  "ps2.5:10.41_0": "Vimuttaṭṭhena vimokkho virāgo.",
  "ps2.5:10.42_0": "Paṭivedhaṭṭhena vijjā virāgo.",
  "ps2.5:10.43_0": "Pariccāgaṭṭhena vimutti virāgo.",
  "ps2.5:10.44_0": "Samucchedaṭṭhena khaye ñāṇaṁ virāgo.",
  "ps2.5:10.45_0": "Chando mūlaṭṭhena virāgo.",
  "ps2.5:10.46_0": "Manasikāro samuṭṭhānaṭṭhena virāgo.",
  "ps2.5:10.47_0": "Phasso samodhānaṭṭhena virāgo.",
  "ps2.5:10.48_0": "Vedanā samosaraṇaṭṭhena virāgo.",
  "ps2.5:10.49_0": "Samādhi pamukhaṭṭhena virāgo.",
  "ps2.5:10.50_0": "Sati ādhipateyyaṭṭhena virāgo.",
  "ps2.5:10.51_0": "Paññā taduttaraṭṭhena virāgo.",
  "ps2.5:10.52_0": "Vimutti sāraṭṭhena virāgo.",
  "ps2.5:10.53_0": "Amatogadhaṁ nibbānaṁ pariyosānaṭṭhena maggo.",
  "ps2.5:11.1_0": "Dassanamaggo sammādiṭṭhi,",
  "ps2.5:11.1_1": "abhiniropanamaggo sammāsaṅkappo …pe…",
  "ps2.5:11.2_0": "amatogadhaṁ nibbānaṁ pariyosānaṭṭhena maggo.",
  "ps2.5:11.3_0": "Evaṁ virāgo maggo.",
  "ps2.5:12.1_0": "Kathaṁ **vimutti phalaṁ?**",
  "ps2.5:12.2_0": "Sotāpattiphalakkhaṇe dassanaṭṭhena sammādiṭṭhi micchādiṭṭhiyā vimuttā hoti,",
  "ps2.5:12.2_1": "tadanuvattakakilesehi ca khandhehi ca vimuttā hoti,",
  "ps2.5:12.2_2": "bahiddhā ca sabbanimittehi vimuttā hoti,",
  "ps2.5:12.2_3": "vimutti vimuttārammaṇā vimuttigocarā vimuttiyā samudāgatā vimuttiyā ṭhitā vimuttiyā patiṭṭhitā.",
  "ps2.5:12.3_0": "**Vimuttī**ti dve muttiyo—",
  "ps2.5:12.4_0": "nibbānañca vimutti,",
  "ps2.5:12.4_1": "ye ca nibbānārammaṇatājātā dhammā sabbe ca vimuttā hontīti—",
  "ps2.5:12.5_0": "vimutti phalaṁ.",
  "ps2.5:13.1_0": "Abhiniropanaṭṭhena sammāsaṅkappo micchāsaṅkappā vimutto hoti,",
  "ps2.5:13.1_1": "tadanuvattakakilesehi ca khandhehi ca vimutto hoti,",
  "ps2.5:13.1_2": "bahiddhā ca sabbanimittehi vimutto hoti,",
  "ps2.5:13.1_3": "vimutti vimuttārammaṇā vimuttigocarā vimuttiyā samudāgatā vimuttiyā ṭhitā vimuttiyā patiṭṭhitā.",
  "ps2.5:13.2_0": "**Vimuttī**ti dve vimuttiyo—",
  "ps2.5:13.3_0": "nibbānañca vimutti,",
  "ps2.5:13.3_1": "ye ca nibbānārammaṇatājātā dhammā sabbe vimuttā hontīti—",
  "ps2.5:13.4_0": "vimutti phalaṁ.",
  "ps2.5:14.1_0": "Pariggahaṭṭhena sammāvācā micchāvācāya vimuttā hoti,",
  "ps2.5:14.1_1": "samuṭṭhānaṭṭhena sammākammanto micchākammantā vimutto hoti,",
  "ps2.5:14.1_2": "vodānaṭṭhena sammāājīvo micchāājīvā vimutto hoti,",
  "ps2.5:14.1_3": "paggahaṭṭhena sammāvāyāmo micchāvāyāmā vimutto hoti,",
  "ps2.5:14.1_4": "upaṭṭhānaṭṭhena sammāsati micchāsatiyā vimuttā hoti,",
  "ps2.5:14.1_5": "avikkhepaṭṭhena sammāsamādhi micchāsamādhito vimutto hoti,",
  "ps2.5:14.1_6": "tadanuvattakakilesehi ca khandhehi ca vimutto hoti,",
  "ps2.5:14.1_7": "bahiddhā ca sabbanimittehi vimutto hoti,",
  "ps2.5:14.1_8": "vimutti vimuttārammaṇā vimuttigocarā vimuttiyā samudāgatā vimuttiyā ṭhitā vimuttiyā patiṭṭhitā.",
  "ps2.5:14.2_0": "**Vimuttī**ti dve vimuttiyo—",
  "ps2.5:14.3_0": "nibbānañca vimutti,",
  "ps2.5:14.3_1": "ye ca nibbānārammaṇatājātā dhammā sabbe vimuttā hontīti—",
  "ps2.5:14.4_0": "vimutti phalaṁ.",
  "ps2.5:15.1_0": "Sakadāgāmiphalakkhaṇe dassanaṭṭhena sammādiṭṭhi …pe…",
  "ps2.5:15.2_0": "avikkhepaṭṭhena sammāsamādhi oḷārikā kāmarāgasaññojanā paṭighasaññojanā oḷārikā kāmarāgānusayā paṭighānusayā vimutto hoti,",
  "ps2.5:15.2_1": "tadanuvattakakilesehi ca khandhehi ca vimutto hoti,",
  "ps2.5:15.2_2": "bahiddhā ca sabbanimittehi vimutto hoti,",
  "ps2.5:15.2_3": "vimutti vimuttārammaṇā vimuttigocarā vimuttiyā samudāgatā vimuttiyā ṭhitā vimuttiyā patiṭṭhitā.",
  "ps2.5:15.3_0": "**Vimuttī**ti dve vimuttiyo—",
  "ps2.5:15.4_0": "nibbānañca vimutti,",
  "ps2.5:15.4_1": "ye ca nibbānārammaṇatājātā dhammā sabbe vimuttā hontīti—",
  "ps2.5:15.5_0": "vimutti phalaṁ.",
  "ps2.5:16.1_0": "Anāgāmiphalakkhaṇe dassanaṭṭhena sammādiṭṭhi …pe…",
  "ps2.5:16.2_0": "avikkhepaṭṭhena sammāsamādhi anusahagatā kāmarāgasaññojanā paṭighasaññojanā anusahagatā kāmarāgānusayā paṭighānusayā vimutto hoti,",
  "ps2.5:16.2_1": "tadanuvattakakilesehi ca khandhehi ca vimutto hoti,",
  "ps2.5:16.2_2": "bahiddhā ca sabbanimittehi vimutto hoti,",
  "ps2.5:16.2_3": "vimutti vimuttārammaṇā vimuttigocarā vimuttiyā samudāgatā vimuttiyā ṭhitā vimuttiyā patiṭṭhitā …pe….",
  "ps2.5:17.1_0": "Arahattaphalakkhaṇe dassanaṭṭhena sammādiṭṭhi …pe…",
  "ps2.5:17.2_0": "avikkhepaṭṭhena sammāsamādhi rūparāgā arūparāgā mānā uddhaccā avijjāya mānānusayā bhavarāgānusayā avijjānusayā vimutto hoti,",
  "ps2.5:17.2_1": "tadanuvattakakilesehi ca khandhehi ca vimutto hoti,",
  "ps2.5:17.2_2": "bahiddhā ca sabbanimittehi vimutto hoti,",
  "ps2.5:17.2_3": "vimutti vimuttārammaṇā vimuttigocarā vimuttiyā samudāgatā vimuttiyā ṭhitā vimuttiyā patiṭṭhitā.",
  "ps2.5:17.3_0": "**Vimuttī**ti dve vimuttiyo—",
  "ps2.5:17.4_0": "nibbānañca vimutti,",
  "ps2.5:17.4_1": "ye ca nibbānārammaṇatājātā dhammā sabbe vimuttā hontīti—",
  "ps2.5:17.5_0": "vimutti phalaṁ.",
  "ps2.5:18.1_0": "Dassanavimutti sammādiṭṭhi …pe…",
  "ps2.5:18.2_0": "avikkhepavimutti sammāsamādhi,",
  "ps2.5:18.2_1": "upaṭṭhānavimutti satisambojjhaṅgo,",
  "ps2.5:18.2_2": "paṭisaṅkhānavimutti upekkhāsambojjhaṅgo.",
  "ps2.5:18.3_0": "Assaddhiye akampiyavimutti saddhābalaṁ …pe…",
  "ps2.5:18.4_0": "avijjāya akampiyavimutti paññābalaṁ.",
  "ps2.5:18.5_0": "Adhimokkhavimutti saddhindriyaṁ …pe…",
  "ps2.5:18.6_0": "dassanavimutti paññindriyaṁ.",
  "ps2.5:19.1_0": "Ādhipateyyaṭṭhena indriyā vimutti.",
  "ps2.5:19.2_0": "Akampiyaṭṭhena balā vimutti,",
  "ps2.5:19.2_1": "niyyānaṭṭhena bojjhaṅgā vimutti, hetuṭṭhena maggo vimutti,",
  "ps2.5:19.2_2": "upaṭṭhānaṭṭhena satipaṭṭhānā vimutti,",
  "ps2.5:19.2_3": "padahanaṭṭhena sammappadhānā vimutti,",
  "ps2.5:19.2_4": "ijjhanaṭṭhena iddhipādā vimutti, tathaṭṭhena saccā vimutti,",
  "ps2.5:19.2_5": "avikkhepaṭṭhena samatho vimutti,",
  "ps2.5:19.2_6": "anupassanaṭṭhena vipassanā vimutti,",
  "ps2.5:19.2_7": "ekarasaṭṭhena samathavipassanā vimutti,",
  "ps2.5:19.2_8": "anativattanaṭṭhena yuganaddhaṁ vimutti,",
  "ps2.5:19.2_9": "saṁvaraṭṭhena sīlavisuddhi vimutti,",
  "ps2.5:19.2_10": "avikkhepaṭṭhena cittavisuddhi vimutti,",
  "ps2.5:19.2_11": "dassanaṭṭhena diṭṭhivisuddhi vimutti,",
  "ps2.5:19.2_12": "vimuttaṭṭhena vimokkho vimutti,",
  "ps2.5:19.2_13": "paṭivedhaṭṭhena vijjā vimutti,",
  "ps2.5:19.2_14": "pariccāgaṭṭhena vimutti vimutti,",
  "ps2.5:19.2_15": "paṭippassaddhiyaṭṭhena anuppāde ñāṇaṁ vimutti, chando mūlaṭṭhena vimutti,",
  "ps2.5:19.2_16": "manasikāro samuṭṭhānaṭṭhena vimutti,",
  "ps2.5:19.2_17": "phasso samodhānaṭṭhena vimutti,",
  "ps2.5:19.2_18": "vedanā samosaraṇaṭṭhena vimutti,",
  "ps2.5:19.2_19": "samādhi pamukhaṭṭhena vimutti,",
  "ps2.5:19.2_20": "sati ādhipateyyaṭṭhena vimutti,",
  "ps2.5:19.2_21": "paññā taduttaraṭṭhena vimutti, vimutti sāraṭṭhena vimutti,",
  "ps2.5:19.2_22": "amatogadhaṁ nibbānaṁ pariyosānaṭṭhena vimutti.",
  "ps2.5:19.3_0": "Evaṁ vimutti phalaṁ.",
  "ps2.5:19.4_0": "Evaṁ virāgo maggo, vimutti phalanti.",
  "ps2.5:19.5_0": "Virāgakathā niṭṭhitā."
}