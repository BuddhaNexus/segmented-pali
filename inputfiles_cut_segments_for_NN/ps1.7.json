{
  "ps1.7:0.1_0": "Paṭisambhidāmagga",
  "ps1.7:0.2_0": "1 Mahāvagga",
  "ps1.7:0.3_0": "1.7. Kammakathā",
  "ps1.7:1.1_0": "Ahosi kammaṁ, ahosi kammavipāko.",
  "ps1.7:1.2_0": "Ahosi kammaṁ, nāhosi kammavipāko.",
  "ps1.7:1.3_0": "Ahosi kammaṁ, atthi kammavipāko.",
  "ps1.7:1.4_0": "Ahosi kammaṁ, natthi kammavipāko.",
  "ps1.7:1.5_0": "Ahosi kammaṁ, bhavissati kammavipāko.",
  "ps1.7:1.6_0": "Ahosi kammaṁ, na bhavissati kammavipāko.",
  "ps1.7:1.7_0": "(Atītakammaṁ) ",
  "ps1.7:2.1_0": "Atthi kammaṁ, atthi kammavipāko.",
  "ps1.7:2.2_0": "Atthi kammaṁ, natthi kammavipāko.",
  "ps1.7:2.3_0": "Atthi kammaṁ, bhavissati kammavipāko.",
  "ps1.7:2.4_0": "Atthi kammaṁ, na bhavissati kammavipāko.",
  "ps1.7:2.5_0": "(Paccuppannakammaṁ) ",
  "ps1.7:3.1_0": "Bhavissati kammaṁ, bhavissati kammavipāko.",
  "ps1.7:3.2_0": "Bhavissati kammaṁ, na bhavissati kammavipāko.",
  "ps1.7:3.3_0": "(Anāgatakammaṁ) ",
  "ps1.7:4.1_0": "Ahosi kusalaṁ kammaṁ,",
  "ps1.7:4.1_1": "ahosi kusalassa kammassa vipāko.",
  "ps1.7:4.2_0": "Ahosi kusalaṁ kammaṁ,",
  "ps1.7:4.2_1": "nāhosi kusalassa kammassa vipāko.",
  "ps1.7:4.3_0": "Ahosi kusalaṁ kammaṁ,",
  "ps1.7:4.3_1": "atthi kusalassa kammassa vipāko.",
  "ps1.7:4.4_0": "Ahosi kusalaṁ kammaṁ,",
  "ps1.7:4.4_1": "natthi kusalassa kammassa vipāko.",
  "ps1.7:4.5_0": "Ahosi kusalaṁ kammaṁ,",
  "ps1.7:4.5_1": "bhavissati kusalassa kammassa vipāko.",
  "ps1.7:4.6_0": "Ahosi kusalaṁ kammaṁ,",
  "ps1.7:4.6_1": "na bhavissati kusalassa kammassa vipāko.",
  "ps1.7:5.1_0": "Atthi kusalaṁ kammaṁ,",
  "ps1.7:5.1_1": "atthi kusalassa kammassa vipāko.",
  "ps1.7:5.2_0": "Atthi kusalaṁ kammaṁ,",
  "ps1.7:5.2_1": "natthi kusalassa kammassa vipāko.",
  "ps1.7:5.3_0": "Atthi kusalaṁ kammaṁ,",
  "ps1.7:5.3_1": "bhavissati kusalassa kammassa vipāko.",
  "ps1.7:5.4_0": "Atthi kusalaṁ kammaṁ,",
  "ps1.7:5.4_1": "na bhavissati kusalassa kammassa vipāko.",
  "ps1.7:6.1_0": "Bhavissati kusalaṁ kammaṁ,",
  "ps1.7:6.1_1": "bhavissati kusalassa kammassa vipāko.",
  "ps1.7:6.2_0": "Bhavissati kusalaṁ kammaṁ,",
  "ps1.7:6.2_1": "na bhavissati kusalassa kammassa vipāko.",
  "ps1.7:7.1_0": "Ahosi akusalaṁ kammaṁ,",
  "ps1.7:7.1_1": "ahosi akusalassa kammassa vipāko.",
  "ps1.7:7.2_0": "Ahosi akusalaṁ kammaṁ,",
  "ps1.7:7.2_1": "nāhosi akusalassa kammassa vipāko.",
  "ps1.7:7.3_0": "Ahosi akusalaṁ kammaṁ,",
  "ps1.7:7.3_1": "atthi akusalassa kammassa vipāko.",
  "ps1.7:7.4_0": "Ahosi akusalaṁ kammaṁ,",
  "ps1.7:7.4_1": "natthi akusalassa kammassa vipāko.",
  "ps1.7:7.5_0": "Ahosi akusalaṁ kammaṁ,",
  "ps1.7:7.5_1": "bhavissati akusalassa kammassa vipāko.",
  "ps1.7:7.6_0": "Ahosi akusalaṁ kammaṁ,",
  "ps1.7:7.6_1": "na bhavissati akusalassa kammassa vipāko.",
  "ps1.7:8.1_0": "Atthi akusalaṁ kammaṁ,",
  "ps1.7:8.1_1": "atthi akusalassa kammassa vipāko.",
  "ps1.7:8.2_0": "Atthi akusalaṁ kammaṁ,",
  "ps1.7:8.2_1": "natthi akusalassa kammassa vipāko.",
  "ps1.7:8.3_0": "Atthi akusalaṁ kammaṁ,",
  "ps1.7:8.3_1": "bhavissati akusalassa kammassa vipāko.",
  "ps1.7:8.4_0": "Atthi akusalaṁ kammaṁ,",
  "ps1.7:8.4_1": "na bhavissati akusalassa kammassa vipāko.",
  "ps1.7:9.1_0": "Bhavissati akusalaṁ kammaṁ,",
  "ps1.7:9.1_1": "bhavissati akusalassa kammassa vipāko.",
  "ps1.7:9.2_0": "Bhavissati akusalaṁ kammaṁ,",
  "ps1.7:9.2_1": "na bhavissati akusalassa kammassa vipāko.",
  "ps1.7:10.1_0": "Ahosi sāvajjaṁ kammaṁ …pe…",
  "ps1.7:10.2_0": "ahosi anavajjaṁ kammaṁ …pe…",
  "ps1.7:10.3_0": "ahosi kaṇhaṁ kammaṁ …pe…",
  "ps1.7:10.4_0": "ahosi sukkaṁ kammaṁ …pe…",
  "ps1.7:10.5_0": "ahosi sukhudrayaṁ kammaṁ …pe…",
  "ps1.7:10.6_0": "ahosi dukkhudrayaṁ kammaṁ …pe…",
  "ps1.7:10.7_0": "ahosi sukhavipākaṁ kammaṁ …pe…",
  "ps1.7:10.8_0": "ahosi dukkhavipākaṁ kammaṁ,",
  "ps1.7:10.8_1": "ahosi dukkhavipākassa kammassa vipāko.",
  "ps1.7:10.9_0": "Ahosi dukkhavipākaṁ kammaṁ,",
  "ps1.7:10.9_1": "nāhosi dukkhavipākassa kammassa vipāko.",
  "ps1.7:10.10_0": "Ahosi dukkhavipākaṁ kammaṁ,",
  "ps1.7:10.10_1": "atthi dukkhavipākassa kammassa vipāko.",
  "ps1.7:10.11_0": "Ahosi dukkhavipākaṁ kammaṁ,",
  "ps1.7:10.11_1": "natthi dukkhavipākassa kammassa vipāko.",
  "ps1.7:10.12_0": "Ahosi dukkhavipākaṁ kammaṁ,",
  "ps1.7:10.12_1": "bhavissati dukkhavipākassa kammassa vipāko.",
  "ps1.7:10.13_0": "Ahosi dukkhavipākaṁ kammaṁ,",
  "ps1.7:10.13_1": "na bhavissati dukkhavipākassa kammassa vipāko.",
  "ps1.7:11.1_0": "Atthi dukkhavipākaṁ kammaṁ,",
  "ps1.7:11.1_1": "atthi dukkhavipākassa kammassa vipāko.",
  "ps1.7:11.2_0": "Atthi dukkhavipākaṁ kammaṁ,",
  "ps1.7:11.2_1": "natthi dukkhavipākassa kammassa vipāko.",
  "ps1.7:11.3_0": "Atthi dukkhavipākaṁ kammaṁ,",
  "ps1.7:11.3_1": "bhavissati dukkhavipākassa kammassa vipāko.",
  "ps1.7:11.4_0": "Atthi dukkhavipākaṁ kammaṁ,",
  "ps1.7:11.4_1": "na bhavissati dukkhavipākassa kammassa vipāko.",
  "ps1.7:12.1_0": "Bhavissati dukkhavipākaṁ kammaṁ,",
  "ps1.7:12.1_1": "bhavissati dukkhavipākassa kammassa vipāko.",
  "ps1.7:12.2_0": "Bhavissati dukkhavipākaṁ kammaṁ,",
  "ps1.7:12.2_1": "na bhavissati dukkhavipākassa kammassa vipākoti.",
  "ps1.7:12.3_0": "Kammakathā niṭṭhitā."
}