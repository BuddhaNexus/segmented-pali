{
  "ja224:0.1_0": "Jātaka",
  "ja224:0.2_0": "Dukanipāta",
  "ja224:0.3_0": "Kāsāvavagga",
  "ja224:0.4_0": "4. Kumbhilajātaka",
  "ja224:1.1_0": "“Yassete caturo dhammā,",
  "ja224:1.2_0": "vānarinda yathā tava;",
  "ja224:1.3_0": "Saccaṁ dhammo dhiti cāgo,",
  "ja224:1.4_0": "diṭṭhaṁ so ativattati.",
  "ja224:2.1_0": "Yassa cete na vijjanti,",
  "ja224:2.2_0": "guṇā paramabhaddakā;",
  "ja224:2.3_0": "Saccaṁ dhammo dhiti cāgo,",
  "ja224:2.4_0": "diṭṭhaṁ so nātivattatī”ti.",
  "ja224:3.1_0": "Kumbhilajātakaṁ catutthaṁ."
}