{
  "pli-tv-bu-vb-sk51:0.1_0": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk51:0.2_0": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk51:0.3_0": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk51:0.4_0": "Surusuruvagga",
  "pli-tv-bu-vb-sk51:0.5_0": "51. Surusurukārakasikkhāpada",
  "pli-tv-bu-vb-sk51:1.1_0": "Tena samayena buddho bhagavā kosambiyaṁ viharati ghositārāme.",
  "pli-tv-bu-vb-sk51:1.2_0": "Tena kho pana samayena aññatarena brāhmaṇena saṅghassa payopānaṁ paṭiyattaṁ hoti.",
  "pli-tv-bu-vb-sk51:1.3_0": "Bhikkhū surusurukārakaṁ khīraṁ pivanti.",
  "pli-tv-bu-vb-sk51:1.4_0": "Aññataro naṭapubbako bhikkhu evamāha—",
  "pli-tv-bu-vb-sk51:1.5_0": "“sabboyaṁ maññe saṅgho sītīkato”ti.",
  "pli-tv-bu-vb-sk51:1.6_0": "Ye te bhikkhū appicchā …pe… te ujjhāyanti khiyyanti vipācenti—",
  "pli-tv-bu-vb-sk51:1.7_0": "“kathañhi nāma bhikkhu saṅghaṁ ārabbha davaṁ karissatī”ti …pe…",
  "pli-tv-bu-vb-sk51:1.8_0": "“saccaṁ kira tvaṁ, bhikkhu,",
  "pli-tv-bu-vb-sk51:1.8_1": "saṅghaṁ ārabbha davaṁ akāsī”ti?",
  "pli-tv-bu-vb-sk51:1.9_0": "“Saccaṁ, bhagavā”ti.",
  "pli-tv-bu-vb-sk51:1.10_0": "Vigarahi buddho bhagavā …pe…",
  "pli-tv-bu-vb-sk51:1.11_0": "kathañhi nāma tvaṁ, moghapurisa,",
  "pli-tv-bu-vb-sk51:1.11_1": "saṅghaṁ ārabbha davaṁ karissasi.",
  "pli-tv-bu-vb-sk51:1.12_0": "Netaṁ, moghapurisa, appasannānaṁ vā pasādāya …pe…",
  "pli-tv-bu-vb-sk51:1.13_0": "vigarahitvā …pe…",
  "pli-tv-bu-vb-sk51:1.14_0": "dhammiṁ kathaṁ katvā bhikkhū āmantesi—",
  "pli-tv-bu-vb-sk51:1.15_0": "“na, bhikkhave,",
  "pli-tv-bu-vb-sk51:1.15_1": "buddhaṁ vā dhammaṁ vā saṅghaṁ vā ārabbha davo kātabbo.",
  "pli-tv-bu-vb-sk51:1.16_0": "Yo kareyya, āpatti dukkaṭassā”ti.",
  "pli-tv-bu-vb-sk51:1.17_0": "Atha kho bhagavā taṁ bhikkhuṁ anekapariyāyena vigarahitvā dubbharatāya …pe…",
  "pli-tv-bu-vb-sk51:1.18_0": "evañca pana, bhikkhave,",
  "pli-tv-bu-vb-sk51:1.18_1": "imaṁ sikkhāpadaṁ uddiseyyātha—",
  "pli-tv-bu-vb-sk51:1.19.1_0": "“Na surusurukārakaṁ bhuñjissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk51:1.20_0": "Na surusurukārakaṁ bhuñjitabbaṁ.",
  "pli-tv-bu-vb-sk51:1.21_0": "Yo anādariyaṁ paṭicca surusurukārakaṁ bhuñjati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk51:1.22.1_0": "Anāpatti—",
  "pli-tv-bu-vb-sk51:1.23.1_0": "asañcicca …pe…",
  "pli-tv-bu-vb-sk51:1.24_0": "ādikammikassāti.",
  "pli-tv-bu-vb-sk51:1.25_0": "Paṭhamasikkhāpadaṁ niṭṭhitaṁ."
}