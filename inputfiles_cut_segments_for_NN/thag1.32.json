{
  "thag1.32:0.1_0": "Theragāthā",
  "thag1.32:0.2_0": "Ekakanipāta",
  "thag1.32:0.3_0": "Catutthavagga",
  "thag1.32:0.4_0": "1.32. Suppiyattheragāthā",
  "thag1.32:1.1_0": "“Ajaraṁ jīramānena,",
  "thag1.32:1.2_0": "tappamānena nibbutiṁ;",
  "thag1.32:1.3_0": "Nimiyaṁ paramaṁ santiṁ,",
  "thag1.32:1.4_0": "yogakkhemaṁ anuttaran”ti.",
  "thag1.32:1.5_0": "… Suppiyo thero …."
}