{
  "tha-ap147:0.1_0": "Therāpadāna",
  "tha-ap147:0.2_0": "Chattavagga",
  "tha-ap147:0.3_0": "5 Umāpupphiyattheraapadāna",
  "tha-ap147:1.1_0": "“Nibbute lokamahite,",
  "tha-ap147:1.2_0": "āhutīnaṁ paṭiggahe;",
  "tha-ap147:1.3_0": "Siddhatthamhi bhagavati,",
  "tha-ap147:1.4_0": "mahāthūpamaho ahu.",
  "tha-ap147:2.1_0": "Mahe pavattamānamhi,",
  "tha-ap147:2.2_0": "siddhatthassa mahesino;",
  "tha-ap147:2.3_0": "Umāpupphaṁ gahetvāna,",
  "tha-ap147:2.4_0": "thūpamhi abhiropayiṁ.",
  "tha-ap147:3.1_0": "Catunnavutito kappe,",
  "tha-ap147:3.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap147:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap147:3.4_0": "thūpapūjāyidaṁ phalaṁ.",
  "tha-ap147:4.1_0": "Ito ca navame kappe,",
  "tha-ap147:4.2_0": "somadevasanāmakā;",
  "tha-ap147:4.3_0": "Pañcāsītisu rājāno,",
  "tha-ap147:4.4_0": "cakkavattī mahabbalā.",
  "tha-ap147:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap147:5.2_0": "…pe…",
  "tha-ap147:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap147:6.1_0": "Itthaṁ sudaṁ āyasmā umāpupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap147:7.1_0": "Umāpupphiyattherassāpadānaṁ pañcamaṁ."
}