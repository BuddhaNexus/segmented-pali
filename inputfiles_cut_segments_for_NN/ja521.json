{
  "ja521:0.1_0": "Jātaka",
  "ja521:0.2_0": "Cattālīsanipāta",
  "ja521:0.3_0": "Tesakuṇavagga",
  "ja521:0.4_0": "1. Tesakuṇajātaka",
  "ja521:1.1_0": "Namo tassa Bhagavato Arahato Sammāsambuddhassa.",
  "ja521:2.1_0": "“Vessantaraṁ taṁ pucchāmi,",
  "ja521:2.2_0": "sakuṇa bhaddamatthu te;",
  "ja521:2.3_0": "Rajjaṁ kāretukāmena,",
  "ja521:2.4_0": "kiṁ su kiccaṁ kataṁ varaṁ”.",
  "ja521:3.1_0": "“Cirassaṁ vata maṁ tāto,",
  "ja521:3.2_0": "kaṁso bārāṇasiggaho;",
  "ja521:3.3_0": "Pamatto appamattaṁ maṁ,",
  "ja521:3.4_0": "pitā puttaṁ acodayi.",
  "ja521:4.1_0": "Paṭhameneva vitathaṁ,",
  "ja521:4.2_0": "kodhaṁ hāsaṁ nivāraye;",
  "ja521:4.3_0": "Tato kiccāni kāreyya,",
  "ja521:4.4_0": "taṁ vataṁ āhu khattiya.",
  "ja521:5.1_0": "Yaṁ tvaṁ tāta tapokammaṁ,",
  "ja521:5.2_0": "Pubbe katamasaṁsayaṁ;",
  "ja521:5.3_0": "Ratto duṭṭho ca yaṁ kayirā,",
  "ja521:5.4_0": "Na taṁ kayirā tato puna.",
  "ja521:6.1_0": "Khattiyassa pamattassa,",
  "ja521:6.2_0": "raṭṭhasmiṁ raṭṭhavaḍḍhana;",
  "ja521:6.3_0": "Sabbe bhogā vinassanti,",
  "ja521:6.4_0": "rañño taṁ vuccate aghaṁ.",
  "ja521:7.1_0": "Sirī tāta alakkhī ca,",
  "ja521:7.2_0": "pucchitā etadabravuṁ;",
  "ja521:7.3_0": "Uṭṭhānavīriye pose,",
  "ja521:7.4_0": "ramāhaṁ anusūyake.",
  "ja521:8.1_0": "Usūyake duhadaye,",
  "ja521:8.2_0": "purise kammadussake;",
  "ja521:8.3_0": "Kāḷakaṇṇī mahārāja,",
  "ja521:8.4_0": "ramati cakkabhañjanī.",
  "ja521:9.1_0": "So tvaṁ sabbesu suhadayo,",
  "ja521:9.2_0": "sabbesaṁ rakkhito bhava;",
  "ja521:9.3_0": "Alakkhiṁ nuda mahārāja,",
  "ja521:9.4_0": "lakkhyā bhava nivesanaṁ.",
  "ja521:10.1_0": "Sa lakkhīdhitisampanno,",
  "ja521:10.2_0": "puriso hi mahaggato;",
  "ja521:10.3_0": "Amittānaṁ kāsipati,",
  "ja521:10.4_0": "mūlaṁ aggañca chindati.",
  "ja521:11.1_0": "Sakkopi hi bhūtapati,",
  "ja521:11.2_0": "uṭṭhāne nappamajjati;",
  "ja521:11.3_0": "Sa kalyāṇe dhitiṁ katvā,",
  "ja521:11.4_0": "uṭṭhāne kurute mano.",
  "ja521:12.1_0": "Gandhabbā pitaro devā,",
  "ja521:12.2_0": "sājīvā honti tādino;",
  "ja521:12.3_0": "Uṭṭhāhato appamajjato,",
  "ja521:12.4_0": "anutiṭṭhanti devatā.",
  "ja521:13.1_0": "So appamatto akkuddho,",
  "ja521:13.2_0": "tāta kiccāni kāraya;",
  "ja521:13.3_0": "Vāyamassu ca kiccesu,",
  "ja521:13.4_0": "nālaso vindate sukhaṁ.",
  "ja521:14.1_0": "Tattheva te vattapadā,",
  "ja521:14.2_0": "esāva anusāsanī;",
  "ja521:14.3_0": "Alaṁ mitte sukhāpetuṁ,",
  "ja521:14.4_0": "amittānaṁ dukhāya ca”.",
  "ja521:15.1_0": "“Sakkhisi tvaṁ kuṇḍalini,",
  "ja521:15.2_0": "maññasi khattabandhuni;",
  "ja521:15.3_0": "Rajjaṁ kāretukāmena,",
  "ja521:15.4_0": "kiṁ su kiccaṁ kataṁ varaṁ”.",
  "ja521:16.1_0": "“Dveva tāta padakāni,",
  "ja521:16.2_0": "yattha sabbaṁ patiṭṭhitaṁ;",
  "ja521:16.3_0": "Aladdhassa ca yo lābho,",
  "ja521:16.4_0": "laddhassa cānurakkhaṇā.",
  "ja521:17.1_0": "Amacce tāta jānāhi,",
  "ja521:17.2_0": "dhīre atthassa kovide;",
  "ja521:17.3_0": "Anakkhākitave tāta,",
  "ja521:17.4_0": "asoṇḍe avināsake.",
  "ja521:18.1_0": "Yo ca taṁ tāta rakkheyya,",
  "ja521:18.2_0": "dhanaṁ yañceva te siyā;",
  "ja521:18.3_0": "Sūtova rathaṁ saṅgaṇhe,",
  "ja521:18.4_0": "so te kiccāni kāraye.",
  "ja521:19.1_0": "Susaṅgahitantajano,",
  "ja521:19.2_0": "sayaṁ vittaṁ avekkhiya;",
  "ja521:19.3_0": "Nidhiñca iṇadānañca,",
  "ja521:19.4_0": "na kare parapattiyā.",
  "ja521:20.1_0": "Sayaṁ āyaṁ vayaṁ jaññā,",
  "ja521:20.2_0": "sayaṁ jaññā katākataṁ;",
  "ja521:20.3_0": "Niggaṇhe niggahārahaṁ,",
  "ja521:20.4_0": "paggaṇhe paggahārahaṁ.",
  "ja521:21.1_0": "Sayaṁ jānapadaṁ atthaṁ,",
  "ja521:21.2_0": "anusāsa rathesabha;",
  "ja521:21.3_0": "Mā te adhammikā yuttā,",
  "ja521:21.4_0": "dhanaṁ raṭṭhañca nāsayuṁ.",
  "ja521:22.1_0": "Mā ca vegena kiccāni,",
  "ja521:22.2_0": "karosi kārayesi vā;",
  "ja521:22.3_0": "Vegasā hi kataṁ kammaṁ,",
  "ja521:22.4_0": "mando pacchānutappati.",
  "ja521:23.1_0": "Mā te adhisare muñca,",
  "ja521:23.2_0": "subāḷhamadhikopitaṁ;",
  "ja521:23.3_0": "Kodhasā hi bahū phītā,",
  "ja521:23.4_0": "kulā akulataṁ gatā.",
  "ja521:24.1_0": "Mā tāta issaromhīti,",
  "ja521:24.2_0": "anatthāya patārayi;",
  "ja521:24.3_0": "Itthīnaṁ purisānañca,",
  "ja521:24.4_0": "mā te āsi dukhudrayo.",
  "ja521:25.1_0": "Apetalomahaṁsassa,",
  "ja521:25.2_0": "rañño kāmānusārino;",
  "ja521:25.3_0": "Sabbe bhogā vinassanti,",
  "ja521:25.4_0": "rañño taṁ vuccate aghaṁ.",
  "ja521:26.1_0": "Tattheva te vattapadā,",
  "ja521:26.2_0": "esāva anusāsanī;",
  "ja521:26.3_0": "Dakkhassudāni puññakaro,",
  "ja521:26.4_0": "asoṇḍo avināsako;",
  "ja521:26.5_0": "Sīlavāssu mahārāja,",
  "ja521:26.6_0": "dussīlo vinipātiko”.",
  "ja521:27.1_0": "“Apucchimha kosiyagottaṁ,",
  "ja521:27.2_0": "kuṇḍaliniṁ tatheva ca;",
  "ja521:27.3_0": "Tvaṁ dāni vadehi jambuka,",
  "ja521:27.4_0": "balānaṁ balamuttamaṁ”.",
  "ja521:28.1_0": "“Balaṁ pañcavidhaṁ loke,",
  "ja521:28.2_0": "purisasmiṁ mahaggate;",
  "ja521:28.3_0": "Tattha bāhubalaṁ nāma,",
  "ja521:28.4_0": "carimaṁ vuccate balaṁ.",
  "ja521:29.1_0": "Bhogabalañca dīghāvu,",
  "ja521:29.2_0": "dutiyaṁ vuccate balaṁ;",
  "ja521:29.3_0": "Amaccabalañca dīghāvu,",
  "ja521:29.4_0": "tatiyaṁ vuccate balaṁ.",
  "ja521:30.1_0": "Abhijaccabalaṁ ceva,",
  "ja521:30.2_0": "taṁ catutthaṁ asaṁsayaṁ;",
  "ja521:30.3_0": "Yāni cetāni sabbāni,",
  "ja521:30.4_0": "adhigaṇhāti paṇḍito.",
  "ja521:31.1_0": "Taṁ balānaṁ balaṁ seṭṭhaṁ,",
  "ja521:31.2_0": "aggaṁ paññābalaṁ balaṁ;",
  "ja521:31.3_0": "Paññābalenupatthaddho,",
  "ja521:31.4_0": "atthaṁ vindati paṇḍito.",
  "ja521:32.1_0": "Api ce labhati mando,",
  "ja521:32.2_0": "phītaṁ dharaṇimuttamaṁ;",
  "ja521:32.3_0": "Akāmassa pasayhaṁ vā,",
  "ja521:32.4_0": "añño taṁ paṭipajjati.",
  "ja521:33.1_0": "Abhijātopi ce hoti,",
  "ja521:33.2_0": "rajjaṁ laddhāna khattiyo;",
  "ja521:33.3_0": "Duppañño hi kāsipati,",
  "ja521:33.4_0": "sabbenapi na jīvati.",
  "ja521:34.1_0": "Paññāva sutaṁ vinicchinī,",
  "ja521:34.2_0": "Paññā kitti silokavaḍḍhanī;",
  "ja521:34.3_0": "Paññāsahito naro idha,",
  "ja521:34.4_0": "Api dukkhe sukhāni vindati.",
  "ja521:35.1_0": "Paññañca kho asussūsaṁ,",
  "ja521:35.2_0": "na koci adhigacchati;",
  "ja521:35.3_0": "Bahussutaṁ anāgamma,",
  "ja521:35.4_0": "dhammaṭṭhaṁ avinibbhujaṁ.",
  "ja521:36.1_0": "Yo ca dhammavibhaṅgaññū,",
  "ja521:36.2_0": "kāluṭṭhāyī matandito;",
  "ja521:36.3_0": "Anuṭṭhahati kālena,",
  "ja521:36.4_0": "kammaphalaṁ tassijjhati.",
  "ja521:37.1_0": "Anāyatanasīlassa,",
  "ja521:37.2_0": "anāyatanasevino;",
  "ja521:37.3_0": "Na nibbindiyakārissa,",
  "ja521:37.4_0": "sammadattho vipaccati.",
  "ja521:38.1_0": "Ajjhattañca payuttassa,",
  "ja521:38.2_0": "tathāyatanasevino;",
  "ja521:38.3_0": "Anibbindiyakārissa,",
  "ja521:38.4_0": "sammadattho vipaccati.",
  "ja521:39.1_0": "Yogappayogasaṅkhātaṁ,",
  "ja521:39.2_0": "sambhatassānurakkhaṇaṁ;",
  "ja521:39.3_0": "Tāni tvaṁ tāta sevassu,",
  "ja521:39.4_0": "mā akammāya randhayi;",
  "ja521:39.5_0": "Akammunā hi dummedho,",
  "ja521:39.6_0": "naḷāgāraṁva sīdati”.",
  "ja521:40.1_0": "“Dhammaṁ cara mahārāja,",
  "ja521:40.2_0": "mātāpitūsu khattiya;",
  "ja521:40.3_0": "Idha dhammaṁ caritvāna,",
  "ja521:40.4_0": "rāja saggaṁ gamissasi.",
  "ja521:41.1_0": "Dhammaṁ cara mahārāja,",
  "ja521:41.2_0": "puttadāresu khattiya;",
  "ja521:41.3_0": "Idha dhammaṁ caritvāna,",
  "ja521:41.4_0": "rāja saggaṁ gamissasi.",
  "ja521:42.1_0": "Dhammaṁ cara mahārāja,",
  "ja521:42.2_0": "mittāmaccesu khattiya;",
  "ja521:42.3_0": "Idha dhammaṁ caritvāna,",
  "ja521:42.4_0": "rāja saggaṁ gamissasi.",
  "ja521:43.1_0": "Dhammaṁ cara mahārāja,",
  "ja521:43.2_0": "vāhanesu balesu ca;",
  "ja521:43.3_0": "Idha dhammaṁ caritvāna,",
  "ja521:43.4_0": "rāja saggaṁ gamissasi.",
  "ja521:44.1_0": "Dhammaṁ cara mahārāja,",
  "ja521:44.2_0": "gāmesu nigamesu ca;",
  "ja521:44.3_0": "…pe…",
  "ja521:45.1_0": "Dhammaṁ cara mahārāja,",
  "ja521:45.2_0": "raṭṭhesu janapadesu ca;",
  "ja521:45.3_0": "…pe…",
  "ja521:46.1_0": "Dhammaṁ cara mahārāja,",
  "ja521:46.2_0": "samaṇabrāhmaṇesu ca;",
  "ja521:46.3_0": "…pe…",
  "ja521:47.1_0": "Dhammaṁ cara mahārāja,",
  "ja521:47.2_0": "migapakkhīsu khattiya;",
  "ja521:47.3_0": "Idha dhammaṁ caritvāna,",
  "ja521:47.4_0": "rāja saggaṁ gamissasi.",
  "ja521:48.1_0": "Dhammaṁ cara mahārāja,",
  "ja521:48.2_0": "dhammo ciṇṇo sukhāvaho;",
  "ja521:48.3_0": "Idha dhammaṁ caritvāna,",
  "ja521:48.4_0": "rāja saggaṁ gamissasi.",
  "ja521:49.1_0": "Dhammaṁ cara mahārāja,",
  "ja521:49.2_0": "Saindā devā sabrahmakā;",
  "ja521:49.3_0": "Suciṇṇena divaṁ pattā,",
  "ja521:49.4_0": "Mā dhammaṁ rāja pāmado.",
  "ja521:50.1_0": "Tattheva te vattapadā,",
  "ja521:50.2_0": "Esāva anusāsanī;",
  "ja521:50.3_0": "Sappaññasevī kalyāṇī,",
  "ja521:50.4_0": "Samattaṁ sāma taṁ vidū”ti.",
  "ja521:51.1_0": "Tesakuṇajātakaṁ paṭhamaṁ."
}