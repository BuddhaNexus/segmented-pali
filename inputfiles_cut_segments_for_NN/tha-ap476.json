{
  "tha-ap476:0.1_0": "Therāpadāna",
  "tha-ap476:0.2_0": "Naḷamālivagga",
  "tha-ap476:0.3_0": "4. Sumanabījaniyattheraapadāna",
  "tha-ap476:1.1_0": "“Vipassino bhagavato,",
  "tha-ap476:1.2_0": "bodhiyā pādaputtame;",
  "tha-ap476:1.3_0": "Sumano bījaniṁ gayha,",
  "tha-ap476:1.4_0": "abījiṁ bodhimuttamaṁ.",
  "tha-ap476:2.1_0": "Ekanavutito kappe,",
  "tha-ap476:2.2_0": "abījiṁ bodhimuttamaṁ;",
  "tha-ap476:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap476:2.4_0": "bījanāya idaṁ phalaṁ.",
  "tha-ap476:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap476:3.2_0": "…pe…",
  "tha-ap476:3.3_0": "viharāmi anāsavo.",
  "tha-ap476:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap476:4.2_0": "…pe…",
  "tha-ap476:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap476:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap476:5.2_0": "…pe…",
  "tha-ap476:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap476:6.1_0": "Itthaṁ sudaṁ āyasmā sumanabījaniyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap476:7.1_0": "Sumanabījaniyattherassāpadānaṁ catutthaṁ."
}