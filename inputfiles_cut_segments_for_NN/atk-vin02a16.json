{
  "atk-vin02a16:0_0": "3. Nissaggiyakaṇḍaṁ (bhikkhunīvibhaṅgavaṇṇanā)",
  "atk-vin02a16:1_0": "Paṭhamanissaggiyapācittiyasikkhāpadavaṇṇanā",
  "atk-vin02a16:2_0": "Tiṁsa nissaggiyā dhammā, bhikkhunīnaṁ pakāsitā;",
  "atk-vin02a16:3_0": "Ye tesaṁ dāni bhavati, ayaṁ saṁvaṇṇanākkamo.",
  "atk-vin02a16:4_0": "733.Āmattikāpaṇanti amattāni vuccanti bhājanāni;",
  "atk-vin02a16:4_1": "tāni ye vikkiṇanti,",
  "atk-vin02a16:4_2": "te vuccanti āmattikā;",
  "atk-vin02a16:4_3": "tesaṁ āpaṇo āmattikāpaṇo;",
  "atk-vin02a16:4_4": "taṁ vā pasāressantīti attho.",
  "atk-vin02a16:5_0": "734.Pattasannicayaṁ kareyyāti pattasannidhiṁ kareyya;",
  "atk-vin02a16:5_1": "ekāhaṁ anadhiṭṭhahitvā vā avikappetvā vā pattaṁ ṭhapeyyāti attho.",
  "atk-vin02a16:5_2": "Sesaṁ mahāvibhaṅge vuttanayeneva veditabbaṁ.",
  "atk-vin02a16:5_3": "Ayameva hi viseso –",
  "atk-vin02a16:5_4": "tattha dasāhaṁ parihāro,",
  "atk-vin02a16:5_5": "idha ekāhampi natthi.",
  "atk-vin02a16:5_6": "Sesaṁ tādisameva.",
  "atk-vin02a16:6_0": "Idampi kathinasamuṭṭhānaṁ –",
  "atk-vin02a16:6_1": "kāyavācato kāyavācācittato ca samuṭṭhāti, akiriyaṁ,",
  "atk-vin02a16:6_2": "nosaññāvimokkhaṁ,",
  "atk-vin02a16:6_3": "acittakaṁ,",
  "atk-vin02a16:6_4": "paṇṇattivajjaṁ,",
  "atk-vin02a16:6_5": "kāyakammaṁ,",
  "atk-vin02a16:6_6": "vacīkammaṁ, ticittaṁ,",
  "atk-vin02a16:6_7": "tivedananti.",
  "atk-vin02a16:7_0": "Paṭhamasikkhāpadaṁ.",
  "atk-vin02a16:8_0": "Dutiyanissaggiyapācittiyasikkhāpadavaṇṇanā",
  "atk-vin02a16:9_0": "738. Dutiye – duccoḷāti virūpacoḷā; jiṇṇacoḷāti attho. Apayyāhīti api ayyāhi.",
  "atk-vin02a16:10_0": "740.Ādissadinnanti sampattā bhājentūti vatvāpi idaṁ gaṇassa idaṁ tumhākaṁ dammīti vatvā vā dātukamyatāya",
  "atk-vin02a16:10_1": "pādamūle ṭhapetvā vā dinnampi ādissa dinnaṁ nāma hoti.",
  "atk-vin02a16:10_2": "Etaṁ sabbampi akālacīvaraṁ.",
  "atk-vin02a16:10_3": "Ayyāya dammīti evaṁ paṭiladdhaṁ pana yathādāneyeva upanetabbaṁ.",
  "atk-vin02a16:10_4": "Sesaṁ uttānameva.",
  "atk-vin02a16:11_0": "Tisamuṭṭhānaṁ – kiriyaṁ,",
  "atk-vin02a16:11_1": "saññāvimokkhaṁ,",
  "atk-vin02a16:11_2": "sacittakaṁ,",
  "atk-vin02a16:11_3": "lokavajjaṁ,",
  "atk-vin02a16:11_4": "kāyakammaṁ,",
  "atk-vin02a16:11_5": "vacīkammaṁ,",
  "atk-vin02a16:11_6": "akusalacittaṁ,",
  "atk-vin02a16:11_7": "tivedananti.",
  "atk-vin02a16:12_0": "Dutiyasikkhāpadaṁ.",
  "atk-vin02a16:13_0": "Tatiyanissaggiyapācittiyasikkhāpadavaṇṇanā",
  "atk-vin02a16:14_0": "743-5. Tatiye –",
  "atk-vin02a16:14_1": "handāti gaṇha.",
  "atk-vin02a16:14_2": "Sayaṁ acchindatīti ekaṁ datvā ekaṁ acchindantiyā ekaṁ nissaggiyaṁ,",
  "atk-vin02a16:14_3": "bahūsu bahūni.",
  "atk-vin02a16:14_4": "Sace saṁharitvā ṭhapitāni ekato acchindati,",
  "atk-vin02a16:14_5": "vatthugaṇanāya āpattiyo.",
  "atk-vin02a16:14_6": "Bandhitvā ṭhapitesu pana ekāva āpatti.",
  "atk-vin02a16:14_7": "Sesaṁ uttānameva.",
  "atk-vin02a16:15_0": "Tisamuṭṭhānaṁ – kiriyaṁ,",
  "atk-vin02a16:15_1": "saññāvimokkhaṁ,",
  "atk-vin02a16:15_2": "sacittakaṁ,",
  "atk-vin02a16:15_3": "lokavajjaṁ,",
  "atk-vin02a16:15_4": "kāyakammaṁ,",
  "atk-vin02a16:15_5": "vacīkammaṁ,",
  "atk-vin02a16:15_6": "akusalacittaṁ,",
  "atk-vin02a16:15_7": "dukkhavedananti.",
  "atk-vin02a16:16_0": "Tatiyasikkhāpadaṁ.",
  "atk-vin02a16:17_0": "Catutthanissaggiyapācittiyasikkhāpadavaṇṇanā",
  "atk-vin02a16:18_0": "748.",
  "atk-vin02a16:18_1": "Catutthe –",
  "atk-vin02a16:18_2": "kayenāti mūlena.",
  "atk-vin02a16:18_3": "Na me āvuso sappinā attho;",
  "atk-vin02a16:18_4": "telena me atthoti idaṁ kira sā āhaṭasappiṁ datvā telampi āharissatīti maññamānā āha.",
  "atk-vin02a16:18_5": "Viññāpetvāti jānāpetvā;",
  "atk-vin02a16:18_6": "idaṁ nāma āharāti yācitvā vā.",
  "atk-vin02a16:19_0": "752.Taññeva viññāpetīti yaṁ paṭhamaṁ viññattaṁ taṁ thokaṁ nappahoti,",
  "atk-vin02a16:19_1": "tasmā puna taññeva viññāpetīti attho.",
  "atk-vin02a16:19_2": "Aññañca viññāpetīti sace paṭhamaṁ sappiviññattaṁ,",
  "atk-vin02a16:19_3": "yamakaṁ pacitabbanti ca vejjena vuttattā telena attho hoti,",
  "atk-vin02a16:19_4": "tato telenāpi me atthoti evaṁ aññañca viññāpeti.",
  "atk-vin02a16:19_5": "Ānisaṁsaṁ dassetvāti sace kahāpaṇassa sappi ābhataṁ hoti,",
  "atk-vin02a16:19_6": "iminā mūlena diguṇaṁ telaṁ labbhati,",
  "atk-vin02a16:19_7": "tenāpi ca idaṁ kiccaṁ nipphajjati,",
  "atk-vin02a16:19_8": "tasmā telaṁ āharāti evaṁ ānisaṁsaṁ dassetvā viññāpetīti.",
  "atk-vin02a16:19_9": "Sesaṁ uttānameva.",
  "atk-vin02a16:20_0": "Chasamuṭṭhānaṁ – kiriyaṁ,",
  "atk-vin02a16:20_1": "nosaññāvimokkhaṁ,",
  "atk-vin02a16:20_2": "acittakaṁ,",
  "atk-vin02a16:20_3": "paṇṇattivajjaṁ,",
  "atk-vin02a16:20_4": "kāyakammaṁ,",
  "atk-vin02a16:20_5": "vacīkammaṁ, ticittaṁ,",
  "atk-vin02a16:20_6": "tivedananti.",
  "atk-vin02a16:21_0": "Catutthasikkhāpadaṁ.",
  "atk-vin02a16:22_0": "Pañcamanissaggiyapācittiyasikkhāpadavaṇṇanā",
  "atk-vin02a16:23_0": "753. Pañcame –",
  "atk-vin02a16:23_1": "na me sikkhamāneti idaṁ kira sā kuladhītā ‘‘ayaṁ addhā evaṁ vuttā idaṁ telaṁ ṭhapetvā sappimpi me attano kulagharā āharissatī’’ti maññamānā āha.",
  "atk-vin02a16:23_2": "Cetāpetvāti jānāpetvā icceva attho.",
  "atk-vin02a16:23_3": "Sesaṁ sabbattha catutthasadisamevāti.",
  "atk-vin02a16:24_0": "Pañcamasikkhāpadaṁ.",
  "atk-vin02a16:25_0": "Chaṭṭhanissaggiyapācittiyasikkhāpadavaṇṇanā",
  "atk-vin02a16:26_0": "758. Chaṭṭhe –",
  "atk-vin02a16:26_1": "chandakanti ‘‘idaṁ nāma dhammakiccaṁ karissāma,",
  "atk-vin02a16:26_2": "yaṁ sakkotha;",
  "atk-vin02a16:26_3": "taṁ dethā’’ti evaṁ paresaṁ chandañca ruciñca uppādetvā gahitaparikkhārassetaṁ adhivacanaṁ.",
  "atk-vin02a16:26_4": "Aññadatthikenāti aññassatthāya dinnena.",
  "atk-vin02a16:26_5": "Aññuddisikenāti aññaṁ uddisitvā dinnena.",
  "atk-vin02a16:26_6": "Saṅghikenāti saṅghassa pariccattena.",
  "atk-vin02a16:27_0": "762.Sesakaṁ upanetīti yadatthāya dinno,",
  "atk-vin02a16:27_1": "taṁ cetāpetvā avasesaṁ aññassatthāya upaneti.",
  "atk-vin02a16:27_2": "Sāmike apaloketvāti ‘‘tumhehi cīvaratthāya dinno,",
  "atk-vin02a16:27_3": "amhākañca cīvaraṁ atthi,",
  "atk-vin02a16:27_4": "telādīhi pana attho’’ti evaṁ āpucchitvā upaneti.",
  "atk-vin02a16:27_5": "Āpadāsūti tathārūpesu upaddavesu;",
  "atk-vin02a16:27_6": "bhikkhuniyo vihāraṁ chaḍḍetvā pakkamanti,",
  "atk-vin02a16:27_7": "evarūpāsu āpadāsu yaṁ vā taṁ vā cetāpetuṁ vaṭṭati.",
  "atk-vin02a16:27_8": "Sesaṁ uttānameva.",
  "atk-vin02a16:28_0": "Chasamuṭṭhānaṁ –",
  "atk-vin02a16:28_1": "kiriyākiriyaṁ,",
  "atk-vin02a16:28_2": "nosaññāvimokkhaṁ,",
  "atk-vin02a16:28_3": "acittakaṁ,",
  "atk-vin02a16:28_4": "paṇṇattivajjaṁ,",
  "atk-vin02a16:28_5": "kāyakammaṁ,",
  "atk-vin02a16:28_6": "vacīkammaṁ, ticittaṁ,",
  "atk-vin02a16:28_7": "tivedananti.",
  "atk-vin02a16:29_0": "Chaṭṭhasikkhāpadaṁ.",
  "atk-vin02a16:30_0": "Sattamanissaggiyapācittiyasikkhāpadavaṇṇanā",
  "atk-vin02a16:31_0": "764. Sattame – saññācikenāti sayaṁ yācitakena. Etadevettha nānākaraṇaṁ. Sesaṁ chaṭṭhasadisamevāti.",
  "atk-vin02a16:32_0": "Sattamasikkhāpadaṁ.",
  "atk-vin02a16:33_0": "Aṭṭhamanissaggiyapācittiyasikkhāpadavaṇṇanā",
  "atk-vin02a16:34_0": "769. Aṭṭhame – mahājanikenāti gaṇassa pariccattena. Etadevettha nānākaraṇaṁ.",
  "atk-vin02a16:35_0": "Aṭṭhamasikkhāpadaṁ.",
  "atk-vin02a16:36_0": "Navamanissaggiyapācittiyasikkhāpadavaṇṇanā",
  "atk-vin02a16:37_0": "774. Navamasikkhāpade – saññācikenāti idaṁ padaṁ ito adhikataraṁ.",
  "atk-vin02a16:38_0": "Navamasikkhāpadaṁ.",
  "atk-vin02a16:39_0": "Dasamanissaggiyapācittiyasikkhāpadavaṇṇanā",
  "atk-vin02a16:40_0": "778. Dasame –",
  "atk-vin02a16:40_1": "pariveṇaṁ undriyatīti pariveṇaṁ vinassati;",
  "atk-vin02a16:40_2": "paripatatīti attho.",
  "atk-vin02a16:40_3": "Idañca padaṁ puggalikena saññācikenāti idañca ettakameva nānākaraṇaṁ.",
  "atk-vin02a16:40_4": "Sesaṁ pubbasadisamevāti.",
  "atk-vin02a16:41_0": "Dasamasikkhāpadaṁ.",
  "atk-vin02a16:42_0": "Ekādasamanissaggiyapācittiyasikkhāpadavaṇṇanā",
  "atk-vin02a16:43_0": "784.",
  "atk-vin02a16:43_1": "Ekādasame –",
  "atk-vin02a16:43_2": "garupāvuraṇanti sītakāle pāvuraṇaṁ.",
  "atk-vin02a16:43_3": "Catukkaṁsaparamanti ettha kaṁso nāma catukkahāpaṇiko hoti;",
  "atk-vin02a16:43_4": "tasmā padabhājane ‘‘soḷasakahāpaṇagghanaka’’nti vuttaṁ.",
  "atk-vin02a16:44_0": "Ekādasamasikkhāpadaṁ.",
  "atk-vin02a16:45_0": "Dvādasamanissaggiyapācittiyasikkhāpadavaṇṇanā",
  "atk-vin02a16:46_0": "789. Dvādasame – lahupāvuraṇanti uṇhakāle pāvuraṇaṁ. Sesaṁ sikkhāpadadvayepi uttānameva.",
  "atk-vin02a16:47_0": "Chasamuṭṭhānaṁ – kiriyaṁ,",
  "atk-vin02a16:47_1": "nosaññāvimokkhaṁ,",
  "atk-vin02a16:47_2": "acittakaṁ,",
  "atk-vin02a16:47_3": "paṇṇattivajjaṁ,",
  "atk-vin02a16:47_4": "kāyakammaṁ,",
  "atk-vin02a16:47_5": "vacīkammaṁ, ticittaṁ,",
  "atk-vin02a16:47_6": "tivedananti.",
  "atk-vin02a16:48_0": "Dvādasamasikkhāpadaṁ.",
  "atk-vin02a16:49_0": "Uddiṭṭhā kho ayyāyo tiṁsa nissaggiyā pācittiyā dhammāti ettha mahāvibhaṅge cīvaravaggato dhovanañca paṭiggahaṇañcāti",
  "atk-vin02a16:49_1": "dve sikkhāpadāni apanetvā akālacīvaraṁ kālacīvaranti adhiṭṭhahitvā bhājitasikkhāpadena ca parivattetvā",
  "atk-vin02a16:49_2": "acchinnacīvarena ca paṭhamavaggo pūretabbo.",
  "atk-vin02a16:49_3": "Puna eḷakalomavaggassa ādito satta sikkhāpadāni apanetvā satta aññadatthikāni pakkhipitvā dutiyavaggo pūretabbo.",
  "atk-vin02a16:49_4": "Tatiyavaggato paṭhamapattaṁ vassikasāṭikaṁ āraññakasikkhāpadanti imāni tīṇi apanetvā pattasannicayagarupāvuraṇalahupāvuraṇasikkhāpadehi",
  "atk-vin02a16:49_5": "tatiyavaggo pūretabbo.",
  "atk-vin02a16:49_6": "Iti bhikkhunīnaṁ dvādasa sikkhāpadāni ekatopaññattāni,",
  "atk-vin02a16:49_7": "aṭṭhārasa ubhatopaññattānīti evaṁ sabbepi pātimokkhuddesamaggena ‘‘uddiṭṭhā kho ayyāyo tiṁsa nissaggiyā",
  "atk-vin02a16:49_8": "pācittiyā dhammā’’ti evamettha attho daṭṭhabbo.",
  "atk-vin02a16:49_9": "Sesaṁ vuttanayamevāti.",
  "atk-vin02a16:50_0": "Samantapāsādikāya vinayasaṁvaṇṇanāya bhikkhunīvibhaṅge",
  "atk-vin02a16:51_0": "Tiṁsakavaṇṇanā niṭṭhitā.",
  "atk-vin02a16:52_0": "Nissaggiyakaṇḍaṁ niṭṭhitaṁ."
}