{
  "ps3.8:0.1_0": "Paṭisambhidāmagga",
  "ps3.8:0.2_0": "3 Paññāvagga",
  "ps3.8:0.3_0": "3.8. Satipaṭṭhānakathā",
  "ps3.8:1.1_0": "Sāvatthinidānaṁ.",
  "ps3.8:1.2_0": "“Cattārome, bhikkhave, satipaṭṭhānā.",
  "ps3.8:1.3_0": "Katame cattāro?",
  "ps3.8:1.4_0": "Idha,",
  "ps3.8:1.4_1": "bhikkhave,",
  "ps3.8:1.4_2": "bhikkhu kāye kāyānupassī viharati ātāpī sampajāno satimā,",
  "ps3.8:1.4_3": "vineyya loke abhijjhādomanassaṁ.",
  "ps3.8:1.5_0": "Vedanāsu …",
  "ps3.8:1.6_0": "citte …",
  "ps3.8:1.7_0": "dhammesu dhammānupassī viharati ātāpī sampajāno satimā, vineyya loke abhijjhādomanassaṁ.",
  "ps3.8:1.8_0": "Ime kho, bhikkhave, cattāro satipaṭṭhānā”ti.",
  "ps3.8:2.1_0": "Kathaṁ **kāye kāyānupassī viharati?**",
  "ps3.8:2.2_0": "Idhekacco pathavīkāyaṁ aniccato anupassati, no niccato;",
  "ps3.8:2.3_0": "dukkhato anupassati, no sukhato;",
  "ps3.8:2.4_0": "anattato anupassati, no attato;",
  "ps3.8:2.5_0": "nibbindati, no nandati;",
  "ps3.8:2.6_0": "virajjati, no rajjati;",
  "ps3.8:2.7_0": "nirodheti, no samudeti, paṭinissajjati, no ādiyati.",
  "ps3.8:2.8_0": "Aniccato anupassanto niccasaññaṁ pajahati,",
  "ps3.8:2.8_1": "dukkhato anupassanto sukhasaññaṁ pajahati,",
  "ps3.8:2.8_2": "anattato anupassanto attasaññaṁ pajahati,",
  "ps3.8:2.8_3": "nibbindanto nandiṁ pajahati,",
  "ps3.8:2.8_4": "virajjanto rāgaṁ pajahati,",
  "ps3.8:2.8_5": "nirodhento samudayaṁ pajahati,",
  "ps3.8:2.8_6": "paṭinissajjanto ādānaṁ pajahati.",
  "ps3.8:2.9_0": "Imehi sattahi ākārehi kāyaṁ anupassati.",
  "ps3.8:2.10_0": "Kāyo upaṭṭhānaṁ, no sati.",
  "ps3.8:2.11_0": "Sati upaṭṭhānañceva sati ca.",
  "ps3.8:2.12_0": "Tāya satiyā tena ñāṇena taṁ kāyaṁ anupassati.",
  "ps3.8:2.13_0": "Tena vuccati—",
  "ps3.8:2.14_0": "“kāye kāyānupassanāsatipaṭṭhānā”.",
  "ps3.8:3.1_0": "**Bhāvanā**ti catasso bhāvanā—",
  "ps3.8:3.2_0": "tattha jātānaṁ dhammānaṁ anativattanaṭṭhena bhāvanā,",
  "ps3.8:3.2_1": "indriyānaṁ ekarasaṭṭhena bhāvanā,",
  "ps3.8:3.2_2": "tadupagavīriyavāhanaṭṭhena bhāvanā,",
  "ps3.8:3.2_3": "āsevanaṭṭhena bhāvanā.",
  "ps3.8:4.1_0": "Idhekacco āpokāyaṁ …pe…",
  "ps3.8:4.2_0": "tejokāyaṁ …",
  "ps3.8:4.3_0": "vāyokāyaṁ …",
  "ps3.8:4.4_0": "kesakāyaṁ …",
  "ps3.8:4.5_0": "lomakāyaṁ …",
  "ps3.8:4.6_0": "chavikāyaṁ …",
  "ps3.8:4.7_0": "cammakāyaṁ …",
  "ps3.8:4.8_0": "maṁsakāyaṁ …",
  "ps3.8:4.9_0": "rudhirakāyaṁ …",
  "ps3.8:4.10_0": "nhārukāyaṁ …",
  "ps3.8:4.11_0": "aṭṭhikāyaṁ …",
  "ps3.8:4.12_0": "aṭṭhimiñjakāyaṁ aniccato anupassati, no niccato;",
  "ps3.8:4.13_0": "dukkhato anupassati, no sukhato;",
  "ps3.8:4.14_0": "anattato anupassati, no attato;",
  "ps3.8:4.15_0": "nibbindati, no nandati;",
  "ps3.8:4.16_0": "virajjati, no rajjati, nirodheti, no samudeti;",
  "ps3.8:4.17_0": "paṭinissajjati, no ādiyati.",
  "ps3.8:4.18_0": "Aniccato anupassanto niccasaññaṁ pajahati,",
  "ps3.8:4.18_1": "dukkhato anupassanto sukhasaññaṁ pajahati,",
  "ps3.8:4.18_2": "anattato anupassanto attasaññaṁ pajahati,",
  "ps3.8:4.18_3": "nibbindanto nandiṁ pajahati,",
  "ps3.8:4.18_4": "virajjanto rāgaṁ pajahati,",
  "ps3.8:4.18_5": "nirodhento samudayaṁ pajahati,",
  "ps3.8:4.18_6": "paṭinissajjanto ādānaṁ pajahati.",
  "ps3.8:4.19_0": "Imehi sattahi ākārehi kāyaṁ anupassati.",
  "ps3.8:4.20_0": "Kāyo upaṭṭhānaṁ, no sati.",
  "ps3.8:4.21_0": "Sati upaṭṭhānañceva sati ca.",
  "ps3.8:4.22_0": "Tāya satiyā tena ñāṇena taṁ kāyaṁ anupassati.",
  "ps3.8:4.23_0": "Tena vuccati—",
  "ps3.8:4.24_0": "“kāye kāyānupassanāsatipaṭṭhānā”.",
  "ps3.8:5.1_0": "**Bhāvanā**ti catasso bhāvanā—",
  "ps3.8:5.2_0": "tattha jātānaṁ dhammānaṁ anativattanaṭṭhena bhāvanā,",
  "ps3.8:5.2_1": "indriyānaṁ ekarasaṭṭhena bhāvanā,",
  "ps3.8:5.2_2": "tadupagavīriyavāhanaṭṭhena bhāvanā,",
  "ps3.8:5.2_3": "āsevanaṭṭhena bhāvanā.",
  "ps3.8:5.3_0": "Evaṁ kāye kāyānupassī viharati.",
  "ps3.8:6.1_0": "Kathaṁ **vedanāsu vedanānupassī viharati?**",
  "ps3.8:6.2_0": "Idhekacco sukhaṁ vedanaṁ aniccato anupassati, no niccato …pe…",
  "ps3.8:6.3_0": "paṭinissajjati, no ādiyati.",
  "ps3.8:6.4_0": "Aniccato anupassanto niccasaññaṁ pajahati …pe…",
  "ps3.8:6.5_0": "paṭinissajjanto ādānaṁ pajahati.",
  "ps3.8:6.6_0": "Imehi sattahi ākārehi vedanaṁ anupassati.",
  "ps3.8:6.7_0": "Vedanā upaṭṭhānaṁ, no sati.",
  "ps3.8:6.8_0": "Sati upaṭṭhānañceva sati ca.",
  "ps3.8:6.9_0": "Tāya satiyā tena ñāṇena taṁ vedanaṁ anupassati.",
  "ps3.8:6.10_0": "Tena vuccati—",
  "ps3.8:6.11_0": "“vedanāsu vedanānupassanāsatipaṭṭhānā”.",
  "ps3.8:7.1_0": "**Bhāvanā**ti catasso bhāvanā …pe…",
  "ps3.8:7.2_0": "āsevanaṭṭhena bhāvanā …pe…",
  "ps3.8:7.3_0": "idhekacco dukkhaṁ vedanaṁ …pe…",
  "ps3.8:7.4_0": "adukkhamasukhaṁ vedanaṁ …",
  "ps3.8:7.5_0": "sāmisaṁ sukhaṁ vedanaṁ …",
  "ps3.8:7.6_0": "nirāmisaṁ sukhaṁ vedanaṁ …",
  "ps3.8:7.7_0": "sāmisaṁ dukkhaṁ vedanaṁ …",
  "ps3.8:7.8_0": "nirāmisaṁ dukkhaṁ vedanaṁ …",
  "ps3.8:7.9_0": "sāmisaṁ adukkhamasukhaṁ vedanaṁ …",
  "ps3.8:7.10_0": "nirāmisaṁ adukkhamasukhaṁ vedanaṁ …",
  "ps3.8:7.11_0": "cakkhusamphassajaṁ vedanaṁ …",
