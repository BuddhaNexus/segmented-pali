{
  "ja177:0.1_0": "Jātaka",
  "ja177:0.2_0": "Dukanipāta",
  "ja177:0.3_0": "Kalyāṇavagga",
  "ja177:0.4_0": "7. Tindukajātaka",
  "ja177:1.1_0": "“Dhanuhatthakalāpehi,",
  "ja177:1.2_0": "nettiṁ savaradhāribhi;",
  "ja177:1.3_0": "Samantā parikiṇṇamha,",
  "ja177:1.4_0": "kathaṁ mokkho bhavissati”.",
  "ja177:2.1_0": "“Appeva bahukiccānaṁ,",
  "ja177:2.2_0": "attho jāyetha koci naṁ;",
  "ja177:2.3_0": "Atthi rukkhassa acchinnaṁ,",
  "ja177:2.4_0": "khajjathaññeva tindukan”ti.",
  "ja177:3.1_0": "Tindukajātakaṁ sattamaṁ."
}