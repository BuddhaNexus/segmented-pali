{
  "atk-s0303a5:0_0": "6. Kilesasaṁyuttavaṇṇanā",
  "atk-s0303a5:1_0": "322-331.",
  "atk-s0303a5:1_1": "Kilesasaṁyutte cittasseso upakkilesoti kataracittassa?",
  "atk-s0303a5:1_2": "Catubhūmakacittassa.",
  "atk-s0303a5:1_3": "Tebhūmakacittassa tāva hotu,",
  "atk-s0303a5:1_4": "lokuttarassa kathaṁ upakkileso hotīti?",
  "atk-s0303a5:1_5": "Uppattinivāraṇato.",
  "atk-s0303a5:1_6": "So hi tassa uppajjituṁ appadānena upakkilesoti veditabbo.",
  "atk-s0303a5:1_7": "Nekkhammaninnanti navalokuttaradhammaninnaṁ.",
  "atk-s0303a5:1_8": "Cittanti samathavipassanācittaṁ.",
  "atk-s0303a5:1_9": "Abhiññā sacchikaraṇīyesu dhammesūti paccavekkhaṇañāṇena abhijānitvā sacchikātabbesu chaḷabhiññādhammesu,",
  "atk-s0303a5:1_10": "ekaṁ dhammaṁ vā gaṇhantena nekkhammanti gahetabbaṁ.",
  "atk-s0303a5:1_11": "Sesaṁ sabbattha uttānamevāti.",
  "atk-s0303a5:2_0": "Kilesasaṁyuttavaṇṇanā niṭṭhitā."
}