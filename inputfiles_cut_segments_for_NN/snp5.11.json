{
  "snp5.11:0.1_0": "Sutta Nipāta 5.11",
  "snp5.11:0.2_0": "Kappamāṇavapucchā",
  "snp5.11:1.1_0": "“Majjhe sarasmiṁ tiṭṭhataṁ,",
  "snp5.11:1.2_0": "(iccāyasmā kappo)",
  "snp5.11:1.3_0": "Oghe jāte mahabbhaye;",
  "snp5.11:1.4_0": "Jarāmaccuparetānaṁ,",
  "snp5.11:1.5_0": "Dīpaṁ pabrūhi mārisa;",
  "snp5.11:1.6_0": "Tvañca me dīpamakkhāhi,",
  "snp5.11:1.7_0": "Yathāyidaṁ nāparaṁ siyā”.",
  "snp5.11:2.1_0": "“Majjhe sarasmiṁ tiṭṭhataṁ,",
  "snp5.11:2.2_0": "(kappāti bhagavā)",
  "snp5.11:2.3_0": "Oghe jāte mahabbhaye;",
  "snp5.11:2.4_0": "Jarāmaccuparetānaṁ,",
  "snp5.11:2.5_0": "Dīpaṁ pabrūmi kappa te.",
  "snp5.11:3.1_0": "Akiñcanaṁ anādānaṁ,",
  "snp5.11:3.2_0": "etaṁ dīpaṁ anāparaṁ;",
  "snp5.11:3.3_0": "Nibbānaṁ iti naṁ brūmi,",
  "snp5.11:3.4_0": "jarāmaccuparikkhayaṁ.",
  "snp5.11:4.1_0": "Etadaññāya ye satā,",
  "snp5.11:4.2_0": "Diṭṭhadhammābhinibbutā;",
  "snp5.11:4.3_0": "Na te māravasānugā,",
  "snp5.11:4.4_0": "Na te mārassa paddhagū”ti.",
  "snp5.11:4.5_0": "Kappamāṇavapucchā dasamā."
}