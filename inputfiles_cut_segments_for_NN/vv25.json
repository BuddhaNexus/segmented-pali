{
  "vv25:0.1_0": "Vimānavatthu",
  "vv25:0.2_0": "Itthivimāna",
  "vv25:0.3_0": "Cittalatāvagga",
  "vv25:0.4_0": "8. Niddāvimānavatthu",
  "vv25:1.1_0": "“Abhikkantena vaṇṇena,",
  "vv25:1.2_0": "yā tvaṁ tiṭṭhasi devate;",
  "vv25:1.3_0": "Obhāsentī disā sabbā,",
  "vv25:1.4_0": "osadhī viya tārakā.",
  "vv25:2.1_0": "Kena tetādiso vaṇṇo,",
  "vv25:2.2_0": "…pe…",
  "vv25:2.3_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv25:3.1_0": "Sā devatā attamanā,",
  "vv25:3.2_0": "…pe…",
  "vv25:3.3_0": "yassa kammassidaṁ phalaṁ.",
  "vv25:4.1_0": "“Niddāti mamaṁ aññaṁsu,",
  "vv25:4.2_0": "Rājagahasmiṁ upāsikā;",
  "vv25:4.3_0": "Saddhā sīlena sampannā,",
  "vv25:4.4_0": "Saṁvibhāgaratā sadā.",
  "vv25:5.1_0": "Acchādanañca bhattañca,",
  "vv25:5.2_0": "senāsanaṁ padīpiyaṁ;",
  "vv25:5.3_0": "Adāsiṁ ujubhūtesu,",
  "vv25:5.4_0": "vippasannena cetasā.",
  "vv25:6.1_0": "Cātuddasiṁ pañcadasiṁ,",
  "vv25:6.2_0": "yā ca pakkhassa aṭṭhamī;",
  "vv25:6.3_0": "Pāṭihāriyapakkhañca,",
  "vv25:6.4_0": "aṭṭhaṅgasusamāgataṁ.",
  "vv25:7.1_0": "Uposathaṁ upavasissaṁ,",
  "vv25:7.2_0": "sadā sīlesu saṁvutā;",
  "vv25:7.3_0": "Saññamā saṁvibhāgā ca,",
  "vv25:7.4_0": "vimānaṁ āvasāmahaṁ.",
  "vv25:8.1_0": "Pāṇātipātā viratā,",
  "vv25:8.2_0": "musāvādā ca saññatā;",
  "vv25:8.3_0": "Theyyā ca aticārā ca,",
  "vv25:8.4_0": "majjapānā ca ārakā.",
  "vv25:9.1_0": "Pañcasikkhāpade ratā,",
  "vv25:9.2_0": "ariyasaccāna kovidā;",
  "vv25:9.3_0": "Upāsikā cakkhumato,",
  "vv25:9.4_0": "gotamassa yasassino.",
  "vv25:10.1_0": "Tena metādiso vaṇṇo,",
  "vv25:10.2_0": "…pe…",
  "vv25:10.3_0": "vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv25:11.1_0": "Niddāvimānaṁ aṭṭhamaṁ."
}