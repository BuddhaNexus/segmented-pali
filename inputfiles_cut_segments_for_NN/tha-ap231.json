{
  "tha-ap231:0.1_0": "Therāpadāna",
  "tha-ap231:0.2_0": "Ālambaṇadāyakavagga",
  "tha-ap231:0.3_0": "9 Sukatāveḷiyattheraapadāna",
  "tha-ap231:1.1_0": "“Asito nāma nāmena,",
  "tha-ap231:1.2_0": "mālākāro ahaṁ tadā;",
  "tha-ap231:1.3_0": "Āveḷaṁ paggahetvāna,",
  "tha-ap231:1.4_0": "rañño dātuṁ vajāmahaṁ.",
  "tha-ap231:2.1_0": "Asampattomhi rājānaṁ,",
  "tha-ap231:2.2_0": "addasaṁ sikhināyakaṁ;",
  "tha-ap231:2.3_0": "Haṭṭho haṭṭhena cittena,",
  "tha-ap231:2.4_0": "buddhassa abhiropayiṁ.",
  "tha-ap231:3.1_0": "Ekattiṁse ito kappe,",
  "tha-ap231:3.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap231:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap231:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap231:4.1_0": "Pañcavīse ito kappe,",
  "tha-ap231:4.2_0": "rājāhosiṁ mahabbalo;",
  "tha-ap231:4.3_0": "Vebhāro nāma nāmena,",
  "tha-ap231:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap231:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap231:5.2_0": "…pe…",
  "tha-ap231:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap231:6.1_0": "Itthaṁ sudaṁ āyasmā sukatāveḷiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap231:7.1_0": "Sukatāveḷiyattherassāpadānaṁ navamaṁ."
}