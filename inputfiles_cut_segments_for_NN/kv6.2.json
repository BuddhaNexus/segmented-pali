{
  "kv6.2:0.1_0": "Kathāvatthu",
  "kv6.2:0.2_0": "Niyāmapaṇṇāsaka",
  "kv6.2:0.3_0": "Chaṭṭhavagga",
  "kv6.2:0.4_0": "Paṭiccasamuppādakathā",
  "kv6.2:1.1_0": "Paṭiccasamuppādo asaṅkhatoti?",
  "kv6.2:1.2_0": "Āmantā.",
  "kv6.2:1.3_0": "Nibbānaṁ tāṇaṁ leṇaṁ saraṇaṁ parāyanaṁ accutaṁ amatanti?",
  "kv6.2:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv6.2:1.5_0": "paṭiccasamuppādo asaṅkhato, nibbānaṁ asaṅkhatanti?",
  "kv6.2:1.6_0": "Āmantā.",
  "kv6.2:1.7_0": "Dve asaṅkhatānīti?",
  "kv6.2:1.8_0": "Na hevaṁ vattabbe …pe…",
  "kv6.2:1.9_0": "dve asaṅkhatānīti?",
  "kv6.2:1.10_0": "Āmantā.",
  "kv6.2:1.11_0": "Dve tāṇāni dve leṇāni dve saraṇāni dve parāyanāni dve accutāni dve amatāni dve nibbānānīti?",
  "kv6.2:1.12_0": "Na hevaṁ vattabbe …pe…",
  "kv6.2:1.13_0": "dve nibbānānīti?",
  "kv6.2:1.14_0": "Āmantā.",
  "kv6.2:1.15_0": "Atthi dvinnaṁ nibbānānaṁ uccanīcatā hīnapaṇītatā ukkaṁsāvakaṁso sīmā vā bhedo vā rāji vā antarikā vāti?",
  "kv6.2:1.16_0": "Na hevaṁ vattabbe …pe….",
  "kv6.2:2.1_0": "Paṭiccasamuppādo asaṅkhatoti?",
  "kv6.2:2.2_0": "Āmantā.",
  "kv6.2:2.3_0": "Avijjā asaṅkhatāti?",
  "kv6.2:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv6.2:2.5_0": "avijjā saṅkhatāti?",
  "kv6.2:2.6_0": "Āmantā.",
  "kv6.2:2.7_0": "Paṭiccasamuppādo saṅkhatoti?",
  "kv6.2:2.8_0": "Na hevaṁ vattabbe …pe…",
  "kv6.2:2.9_0": "paṭiccasamuppādo asaṅkhatoti?",
  "kv6.2:2.10_0": "Āmantā.",
  "kv6.2:2.11_0": "Avijjāpaccayā saṅkhārā asaṅkhatāti?",
  "kv6.2:2.12_0": "Na hevaṁ vattabbe …pe…",
  "kv6.2:2.13_0": "avijjāpaccayā saṅkhārā saṅkhatāti?",
  "kv6.2:2.14_0": "Āmantā.",
  "kv6.2:2.15_0": "Paṭiccasamuppādo saṅkhatoti?",
  "kv6.2:2.16_0": "Na hevaṁ vattabbe …pe…",
  "kv6.2:2.17_0": "paṭiccasamuppādo asaṅkhatoti?",
  "kv6.2:2.18_0": "Āmantā.",
  "kv6.2:2.19_0": "Saṅkhārapaccayā viññāṇaṁ asaṅkhatanti?",
  "kv6.2:2.20_0": "Na hevaṁ vattabbe …pe…",
  "kv6.2:2.21_0": "saṅkhārapaccayā viññāṇaṁ saṅkhatanti?",
  "kv6.2:2.22_0": "Āmantā.",
  "kv6.2:2.23_0": "Paṭiccasamuppādo saṅkhatoti?",
  "kv6.2:2.24_0": "Na hevaṁ vattabbe …pe…",
  "kv6.2:2.25_0": "paṭiccasamuppādo asaṅkhatoti?",
  "kv6.2:2.26_0": "Āmantā.",
  "kv6.2:2.27_0": "Viññāṇapaccayā nāmarūpaṁ asaṅkhatanti?",
  "kv6.2:2.28_0": "Na hevaṁ vattabbe …pe…",
  "kv6.2:2.29_0": "viññāṇapaccayā nāmarūpaṁ saṅkhatanti?",
  "kv6.2:2.30_0": "Āmantā.",
  "kv6.2:2.31_0": "Paṭiccasamuppādo saṅkhatoti?",
  "kv6.2:2.32_0": "Na hevaṁ vattabbe …pe…",
  "kv6.2:2.33_0": "paṭiccasamuppādo asaṅkhatoti?",
  "kv6.2:2.34_0": "Āmantā.",
  "kv6.2:2.35_0": "Jātipaccayā jarāmaraṇaṁ asaṅkhatanti?",
  "kv6.2:2.36_0": "Na hevaṁ vattabbe …pe…",
  "kv6.2:2.37_0": "jātipaccayā jarāmaraṇaṁ saṅkhatanti?",
  "kv6.2:2.38_0": "Āmantā.",
  "kv6.2:2.39_0": "Paṭiccasamuppādo saṅkhatoti?",
  "kv6.2:2.40_0": "Na hevaṁ vattabbe …pe….",
  "kv6.2:3.1_0": "Na vattabbaṁ—",
  "kv6.2:3.2_0": "“paṭiccasamuppādo asaṅkhato”ti?",
  "kv6.2:3.3_0": "Āmantā.",
  "kv6.2:3.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv6.2:3.5_0": "“jātipaccayā, bhikkhave, jarāmaraṇaṁ.",
  "kv6.2:3.6_0": "Uppādā vā tathāgatānaṁ anuppādā vā tathāgatānaṁ ṭhitāva sā dhātu dhammaṭṭhitatā dhammaniyāmatā idappaccayatā.",
  "kv6.2:3.7_0": "Taṁ tathāgato abhisambujjhati abhisameti.",
  "kv6.2:3.8_0": "Abhisambujjhitvā abhisametvā ācikkhati deseti paññāpeti paṭṭhapeti vivarati vibhajati uttāniṁ karoti.",
  "kv6.2:3.9_0": "‘Passathā’ti cāha—",
  "kv6.2:3.10_0": "jātipaccayā, bhikkhave, jarāmaraṇaṁ.",
  "kv6.2:3.11_0": "Bhavapaccayā, bhikkhave, jāti …pe…",
  "kv6.2:3.12_0": "avijjāpaccayā, bhikkhave, saṅkhārā.",
  "kv6.2:3.13_0": "Uppādā vā tathāgatānaṁ anuppādā vā tathāgatānaṁ ṭhitāva sā dhātu …pe…",
  "kv6.2:3.14_0": "‘passathā’ti cāha—",
  "kv6.2:3.15_0": "avijjāpaccayā, bhikkhave, saṅkhārā.",
  "kv6.2:3.16_0": "Iti kho, bhikkhave,",
  "kv6.2:3.16_1": "yā tatra tathatā avitathatā anaññathatā idappaccayatā—",
  "kv6.2:3.17_0": "ayaṁ vuccati, bhikkhave, paṭiccasamuppādo”ti.",
  "kv6.2:3.18_0": "Attheva suttantoti?",
  "kv6.2:3.19_0": "Āmantā.",
  "kv6.2:3.20_0": "Tena hi paṭiccasamuppādo asaṅkhatoti.",
  "kv6.2:4.1_0": "“Avijjāpaccayā saṅkhārā”ti yā tattha dhammaṭṭhitatā dhammaniyāmatā asaṅkhatā, nibbānaṁ asaṅkhatanti?",
  "kv6.2:4.2_0": "Āmantā.",
  "kv6.2:4.3_0": "Dve asaṅkhatānīti?",
  "kv6.2:4.4_0": "Na hevaṁ vattabbe …pe…",
  "kv6.2:4.5_0": "dve asaṅkhatānīti?",
  "kv6.2:4.6_0": "Āmantā.",
  "kv6.2:4.7_0": "Dve tāṇāni …pe…",
  "kv6.2:4.8_0": "antarikā vāti?",
  "kv6.2:4.9_0": "Na hevaṁ vattabbe …pe….",
  "kv6.2:5.1_0": "“Avijjāpaccayā saṅkhārā”ti yā tattha dhammaṭṭhitatā dhammaniyāmatā asaṅkhatā,",
  "kv6.2:5.1_1": "“saṅkhārapaccayā viññāṇan”ti yā tattha dhammaṭṭhitatā dhammaniyāmatā asaṅkhatā, nibbānaṁ asaṅkhatanti?",
  "kv6.2:5.2_0": "Āmantā.",
  "kv6.2:5.3_0": "Tīṇi asaṅkhatānīti?",
  "kv6.2:5.4_0": "Na hevaṁ vattabbe …pe….",
  "kv6.2:5.5_0": "Tīṇi asaṅkhatānīti?",
  "kv6.2:5.6_0": "Āmantā.",
  "kv6.2:5.7_0": "Tīṇi tāṇāni …pe…",
  "kv6.2:5.8_0": "antarikā vāti?",
  "kv6.2:5.9_0": "Na hevaṁ vattabbe …pe….",
  "kv6.2:6.1_0": "“Avijjāpaccayā saṅkhārā”ti yā tattha dhammaṭṭhitatā dhammaniyāmatā asaṅkhatā,",
  "kv6.2:6.1_1": "“saṅkhārapaccayā viññāṇan”ti yā tattha dhammaṭṭhitatā dhammaniyāmatā asaṅkhatā …pe…",
  "kv6.2:6.2_0": "“jātipaccayā jarāmaraṇan”ti yā tattha dhammaṭṭhitatā dhammaniyāmatā asaṅkhatā, nibbānaṁ asaṅkhatanti?",
  "kv6.2:6.3_0": "Āmantā.",
  "kv6.2:6.4_0": "Dvādasa asaṅkhatānīti?",
  "kv6.2:6.5_0": "Na hevaṁ vattabbe …pe…",
  "kv6.2:6.6_0": "dvādasa asaṅkhatānīti?",
  "kv6.2:6.7_0": "Āmantā.",
  "kv6.2:6.8_0": "Dvādasa tāṇāni dvādasa leṇāni …pe…",
  "kv6.2:6.9_0": "antarikā vāti?",
  "kv6.2:6.10_0": "Na hevaṁ vattabbe …pe….",
  "kv6.2:6.11_0": "Paṭiccasamuppādakathā niṭṭhitā."
}