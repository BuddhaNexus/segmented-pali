{
  "tika-abh07t0:0_0": " Namo tassa bhagavato arahato sammāsambuddhassa",
  "tika-abh07t0:1_0": "Abhidhammatthasaṅgaho",
  "tika-abh07t0:2_0": "Ganthārambhakathā",
  "tika-abh07t0:3_0": "1. Sammāsambuddhamatulaṁ , sasaddhammagaṇuttamaṁ.",
  "tika-abh07t0:4_0": "Abhivādiya bhāsissaṁ, abhidhammatthasaṅgahaṁ.",
  "tika-abh07t0:5_0": "Catuparamatthadhammo",
  "tika-abh07t0:6_0": "2. Tattha vuttābhidhammatthā, catudhā paramatthato.",
  "tika-abh07t0:7_0": "Cittaṁ cetasikaṁ rūpaṁ, nibbānamiti sabbathā.",
  "tika-abh07t0:8_0": "1. Cittaparicchedo",
  "tika-abh07t0:9_0": "Bhūmibhedacittaṁ",
  "tika-abh07t0:10_0": "3.",
  "tika-abh07t0:10_1": "Tattha cittaṁ tāva catubbidhaṁ hoti kāmāvacaraṁ rūpāvacaraṁ arūpāvacaraṁ lokuttarañceti.",
  "tika-abh07t0:11_0": "Akusalacittaṁ",
  "tika-abh07t0:12_0": "4. Tattha katamaṁ kāmāvacaraṁ?",
  "tika-abh07t0:12_1": "Somanassasahagataṁ diṭṭhigatasampayuttaṁ asaṅkhārikamekaṁ, sasaṅkhārikamekaṁ,",
  "tika-abh07t0:12_2": "somanassasahagataṁ diṭṭhigatavippayuttaṁ asaṅkhārikamekaṁ, sasaṅkhārikamekaṁ ,",
  "tika-abh07t0:12_3": "upekkhāsahagataṁ diṭṭhigatasampayuttaṁ asaṅkhārikamekaṁ, sasaṅkhārikamekaṁ,",
  "tika-abh07t0:12_4": "upekkhāsahagataṁ diṭṭhigatavippayuttaṁ asaṅkhārikamekaṁ,",
  "tika-abh07t0:12_5": "sasaṅkhārikamekanti imāni aṭṭhapi lobhasahagatacittāni nāma.",
  "tika-abh07t0:13_0": "5.",
  "tika-abh07t0:13_1": "Domanassasahagataṁ paṭighasampayuttaṁ asaṅkhārikamekaṁ,",
  "tika-abh07t0:13_2": "sasaṅkhārikamekanti imāni dvepi paṭighasampayuttacittāni nāma.",
  "tika-abh07t0:14_0": "6.",
  "tika-abh07t0:14_1": "Upekkhāsahagataṁ vicikicchāsampayuttamekaṁ,",
  "tika-abh07t0:14_2": "upekkhāsahagataṁ uddhaccasampayuttamekanti imāni dvepi momūhacittāni nāma.",
  "tika-abh07t0:15_0": "7.",
  "tika-abh07t0:15_1": "Iccevaṁ sabbathāpi dvādasākusalacittāni samattāni.",
  "tika-abh07t0:16_0": "8. Aṭṭhadhā lobhamūlāni, dosamūlāni ca dvidhā.",
  "tika-abh07t0:17_0": "Mohamūlāni ca dveti, dvādasākusalā siyuṁ.",
  "tika-abh07t0:18_0": "Ahetukacittaṁ",
  "tika-abh07t0:19_0": "9.",
  "tika-abh07t0:19_1": "Upekkhāsahagataṁ cakkhuviññāṇaṁ, tathā sotaviññāṇaṁ, ghānaviññāṇaṁ, jivhāviññāṇaṁ ,",
  "tika-abh07t0:19_2": "dukkhasahagataṁ kāyaviññāṇaṁ,",
  "tika-abh07t0:19_3": "upekkhāsahagataṁ sampaṭicchanacittaṁ,",
  "tika-abh07t0:19_4": "upekkhāsahagataṁ santīraṇacittañceti imāni sattapi akusalavipākacittāni nāma.",
  "tika-abh07t0:20_0": "10.",
  "tika-abh07t0:20_1": "Upekkhāsahagataṁ kusalavipākaṁ cakkhuviññāṇaṁ, tathā sotaviññāṇaṁ, ghānaviññāṇaṁ, jivhāviññāṇaṁ, sukhasahagataṁ kāyaviññāṇaṁ,",
  "tika-abh07t0:20_2": "upekkhāsahagataṁ sampaṭicchanacittaṁ,",
  "tika-abh07t0:20_3": "somanassasahagataṁ santīraṇacittaṁ,",
  "tika-abh07t0:20_4": "upekkhāsahagataṁ santīraṇacittañceti imāni aṭṭhapi kusalavipākāhetukacittāni nāma.",
  "tika-abh07t0:21_0": "11.",
  "tika-abh07t0:21_1": "Upekkhāsahagataṁ pañcadvārāvajjanacittaṁ,",
  "tika-abh07t0:21_2": "tathā manodvārāvajjanacittaṁ,",
  "tika-abh07t0:21_3": "somanassasahagataṁ hasituppādacittañceti imāni tīṇipi ahetukakiriyacittāni nāma.",
  "tika-abh07t0:22_0": "12.",
  "tika-abh07t0:22_1": "Icceva sabbathāpi aṭṭhārasāhetukacittāni samattāni.",
  "tika-abh07t0:23_0": "13. Sattākusalapākāni, puññapākāni aṭṭhadhā.",
  "tika-abh07t0:24_0": "Kriyacittāni tīṇīti, aṭṭhārasa ahetukā.",
  "tika-abh07t0:25_0": "Sobhanacittaṁ",
  "tika-abh07t0:26_0": "14. Pāpāhetukamuttāni, sobhanānīti vuccare.",
  "tika-abh07t0:27_0": "Ekūnasaṭṭhi cittāni, athekanavutīpi vā.",
  "tika-abh07t0:28_0": "Kāmāvacarasobhanacittaṁ",
  "tika-abh07t0:29_0": "15.",
  "tika-abh07t0:29_1": "Somanassasahagataṁ ñāṇasampayuttaṁ asaṅkhārikamekaṁ, sasaṅkhārikamekaṁ,",
  "tika-abh07t0:29_2": "somanassasahagataṁ ñāṇavippayuttaṁ asaṅkhārikamekaṁ, sasaṅkhārikamekaṁ,",
  "tika-abh07t0:29_3": "upekkhāsahagataṁ ñāṇasampayuttaṁ asaṅkhārikamekaṁ, sasaṅkhārikamekaṁ.",
  "tika-abh07t0:29_4": "Upekkhāsahagataṁ ñāṇavippayuttaṁ asaṅkhārikamekaṁ,",
  "tika-abh07t0:29_5": "sasaṅkhārikamekanti imāni aṭṭhapi kāmāvacarakusalacittāni nāma.",
  "tika-abh07t0:30_0": "16.",
  "tika-abh07t0:30_1": "Somanassasahagataṁ ñāṇasampayuttaṁ asaṅkhārikamekaṁ, sasaṅkhārikamekaṁ,",
  "tika-abh07t0:30_2": "somanassasahagataṁ ñāṇavippayuttaṁ asaṅkhārikamekaṁ, sasaṅkhārikamekaṁ,",
  "tika-abh07t0:30_3": "upekkhāsahagataṁ ñāṇasampayuttaṁ asaṅkhārikamekaṁ, sasaṅkhārikamekaṁ,",
  "tika-abh07t0:30_4": "upekkhāsahagataṁ ñāṇavippayuttaṁ asaṅkhārikamekaṁ,",
  "tika-abh07t0:30_5": "sasaṅkhārikamekanti imāni aṭṭhapi sahetukakāmāvacaravipākacittāni nāma.",
  "tika-abh07t0:31_0": "17.",
  "tika-abh07t0:31_1": "Somassasahagataṁ ñāṇasampayuttaṁ asaṅkhārikamekaṁ, sasaṅkhārikamekaṁ,",
  "tika-abh07t0:31_2": "somanassasahagataṁ ñāṇavippayuttaṁ asaṅkhārikamekaṁ , sasaṅkhārikamekaṁ,",
  "tika-abh07t0:31_3": "upekkhāsahagataṁ ñāṇasampayuttaṁ asaṅkhārikamekaṁ, sasaṅkhārikamekaṁ,",
  "tika-abh07t0:31_4": "upekkhāsahagataṁ ñāṇavippayuttaṁ asaṅkhārikamekaṁ,",
  "tika-abh07t0:31_5": "sasaṅkhārikamekanti imāni aṭṭhapi sahetukakāmāvacarakiriyacittāni nāma.",
  "tika-abh07t0:32_0": "18.",
  "tika-abh07t0:32_1": "Iccevaṁ sabbathāpi catuvīsati sahetukakāmāvacarakusalavipākakiriyacittāni samattāni.",
  "tika-abh07t0:33_0": "19. Vedanāñāṇasaṅkhārabhedena catuvīsati.",
  "tika-abh07t0:34_0": "Sahetukāmāvacarapuññapākakriyā matā.",
  "tika-abh07t0:35_0": "20. Kāme tevīsa pākāni, puññāpuññāni vīsati.",
  "tika-abh07t0:36_0": "Ekādasa kriyā ceti, catupaññāsa sabbathā.",
  "tika-abh07t0:37_0": "Rūpāvacaracittaṁ",
  "tika-abh07t0:38_0": "21.",
  "tika-abh07t0:38_1": "Vitakkavicārapītisukhekaggatāsahitaṁ paṭhamajjhānakusalacittaṁ,",
  "tika-abh07t0:38_2": "vicārapītisukhekaggatāsahitaṁ dutiyajjhānakusalacittaṁ,",
  "tika-abh07t0:38_3": "pītisukhekaggatāsahitaṁ tatiyajjhānakusalacittaṁ,",
  "tika-abh07t0:38_4": "sukhekaggatāsahitaṁ catutthajjhānakusalacittaṁ,",
  "tika-abh07t0:38_5": "upekkhekaggatāsahitaṁ pañcamajjhānakusalacittañceti imāni pañcapi rūpāvacarakusalacittāni nāma.",
  "tika-abh07t0:39_0": "22.",
  "tika-abh07t0:39_1": "Vitakkavicārapītisukhekaggatāsahitaṁ paṭhamajjhānavipākacittaṁ,",
  "tika-abh07t0:39_2": "vicārapītisukhekaggatāsahitaṁ dutiyajjhānavipākacittaṁ,",
  "tika-abh07t0:39_3": "pītisukhekaggatāsahitaṁ tatiyajjhānavipākacittaṁ,",
  "tika-abh07t0:39_4": "sukhekaggatāsahitaṁ catutthajjhānavipākacittaṁ,",
  "tika-abh07t0:39_5": "upekkhekaggatāsahitaṁ pañcamajjhānavipākacittañceti imāni pañcapi rūpāvacaravipākacittāni nāma.",
  "tika-abh07t0:40_0": "23.",
  "tika-abh07t0:40_1": "Vitakkavicārapītisukhekaggatāsahitaṁ paṭhamajjhānakiriyacittaṁ,",
  "tika-abh07t0:40_2": "vicārapītisukhekaggatāsahitaṁ dutiyajjhānakiriyacittaṁ,",
  "tika-abh07t0:40_3": "pītisukhekaggatāsahitaṁ tatiyajjhānakiriyacittaṁ ,",
  "tika-abh07t0:40_4": "sukhekaggatāsahitaṁ catutthajjhānakiriyacittaṁ,",
  "tika-abh07t0:40_5": "upekkhekaggatāsahitaṁ pañcamajjhānakiriyacittañceti imāni pañcapi rūpāvacarakiriyacittāni nāma.",
  "tika-abh07t0:41_0": "24.",
  "tika-abh07t0:41_1": "Iccevaṁ sabbathāpi pannarasa rūpāvacarakusalavipākakiriyacittāni samattāni.",
  "tika-abh07t0:42_0": "25. Pañcadhā jhānabhedena, rūpāvacaramānasaṁ.",
  "tika-abh07t0:43_0": "Puññapākakriyābhedā, taṁ pañcadasadhā bhave.",
  "tika-abh07t0:44_0": "Arūpāvacaracittaṁ",
  "tika-abh07t0:45_0": "26.",
  "tika-abh07t0:45_1": "Ākāsānañcāyatanakusalacittaṁ,",
  "tika-abh07t0:45_2": "viññāṇañcāyatanakusalacittaṁ, ākiñcaññāyatanakusalacittaṁ,",
  "tika-abh07t0:45_3": "nevasaññānāsaññāyatanakusalacittañceti imāni cattāripi arūpāvacarakusalacittāni nāma.",
  "tika-abh07t0:46_0": "27.",
  "tika-abh07t0:46_1": "Ākāsānañcāyatanavipākacittaṁ,",
  "tika-abh07t0:46_2": "viññāṇañcāyatanavipākacittaṁ, ākiñcaññāyatanavipākacittaṁ,",
  "tika-abh07t0:46_3": "nevasaññānāsaññāyatanavipākacittañceti imāni cattāripi arūpāvacaravipākacittāni nāma.",
  "tika-abh07t0:47_0": "28.",
  "tika-abh07t0:47_1": "Ākāsānañcāyatanakiriyacittaṁ,",
  "tika-abh07t0:47_2": "viññāṇañcāyatanakiriyacittaṁ, ākiñcaññāyatanakiriyacittaṁ,",
  "tika-abh07t0:47_3": "nevasaññānāsaññāyatanakiriyacittañceti imāni cattāripi arūpāvacarakiriyacittāni nāma.",
  "tika-abh07t0:48_0": "29.",
  "tika-abh07t0:48_1": "Iccevaṁ sabbathāpi dvādasa arūpāvacarakusalavipākakiriyacittāni samattāni.",
  "tika-abh07t0:49_0": "30. Ālambaṇappabhedena , catudhāruppamānasaṁ.",
  "tika-abh07t0:50_0": "Puññapākakriyābhedā, puna dvādasadhā ṭhitaṁ.",
  "tika-abh07t0:51_0": "Lokuttaracittaṁ",
  "tika-abh07t0:52_0": "31. Sotāpattimaggacittaṁ , sakadāgāmimaggacittaṁ, anāgāmimaggacittaṁ,",
  "tika-abh07t0:52_1": "arahattamaggacittañceti imāni cattāripi lokuttarakusalacittāni nāma.",
  "tika-abh07t0:53_0": "32. Sotāpattiphalacittaṁ, sakadāgāmiphalacittaṁ, anāgāmiphalacittaṁ,",
  "tika-abh07t0:53_1": "arahattaphalacittañceti imāni cattāripi lokuttaravipākacittāni nāma.",
  "tika-abh07t0:54_0": "33.",
  "tika-abh07t0:54_1": "Iccevaṁ sabbathāpi aṭṭha lokuttarakusalavipākacittāni samattāni.",
  "tika-abh07t0:55_0": "34. Catumaggappabhedena, catudhā kusalaṁ tathā.",
  "tika-abh07t0:56_0": "Pākaṁ tassa phalattāti, aṭṭhadhānuttaraṁ mataṁ.",
  "tika-abh07t0:57_0": "Cittagaṇanasaṅgaho",
  "tika-abh07t0:58_0": "35. Dvādasākusalānevaṁ, kusalānekavīsati.",
  "tika-abh07t0:59_0": "Chattiṁseva vipākāni, kriyacittāni vīsati.",
  "tika-abh07t0:60_0": "36. Catupaññāsadhā kāme, rūpe pannarasīraye.",
  "tika-abh07t0:61_0": "Cittāni dvādasāruppe, aṭṭhadhānuttare tathā.",
  "tika-abh07t0:62_0": "37. Itthamekūnanavutipabhedaṁ pana mānasaṁ.",
  "tika-abh07t0:63_0": "Ekavīsasataṁ vātha, vibhajanti vicakkhaṇā.",
  "tika-abh07t0:64_0": "Vitthāragaṇanā",
  "tika-abh07t0:65_0": "38.",
  "tika-abh07t0:65_1": "Kathamekūnanavutividhaṁ cittaṁ ekavīsasataṁ hoti?",
  "tika-abh07t0:65_2": "Vitakkavicārapītisukhekaggatāsahitaṁ paṭhamajjhānasotāpattimaggacittaṁ,",
  "tika-abh07t0:65_3": "vicārapītisukhekaggatāsahitaṁ dutiyajjhānasotāpattimaggacittaṁ,",
  "tika-abh07t0:65_4": "pītisukhekaggatāsahitaṁ tatiyajjhānasotāpattimaggacittaṁ,",
  "tika-abh07t0:65_5": "sukhekaggatāsahitaṁ catutthajjhānasotāpattimaggacittaṁ,",
  "tika-abh07t0:65_6": "upekkhekaggatāsahitaṁ pañcamajjhānasotāpattimaggacittañceti imāni pañcapi sotāpattimaggacittāni nāma.",
  "tika-abh07t0:66_0": "39.",
  "tika-abh07t0:66_1": "Tathā sakadāgāmimaggaanāgāmimaggaarahattamaggacittañceti samavīsati maggacittāni.",
  "tika-abh07t0:67_0": "40.",
  "tika-abh07t0:67_1": "Tathā phalacittāni ceti samacattālīsa lokuttaracittāni bhavantīti.",
  "tika-abh07t0:68_0": "41. Jhānaṅgayogabhedena, katvekekantu pañcadhā.",
  "tika-abh07t0:69_0": "Vuccatānuttaraṁ cittaṁ, cattālīsavidhanti ca.",
  "tika-abh07t0:70_0": "42. Yathā ca rūpāvacaraṁ, gayhatānuttaraṁ tathā.",
  "tika-abh07t0:71_0": "Paṭhamādijhānabhede, āruppañcāpi pañcame.",
  "tika-abh07t0:72_0": "Ekādasavidhaṁ tasmā, paṭhamādikamīritaṁ;",
  "tika-abh07t0:73_0": "Jhānamekekamante tu, tevīsatividhaṁ bhave.",
  "tika-abh07t0:74_0": "43. Sattatiṁsavidhaṁ puññaṁ, dvipaññāsavidhaṁ tathā.",
  "tika-abh07t0:75_0": "Pākamiccāhu cittāni, ekavīsasataṁ budhā.",
  "tika-abh07t0:76_0": "Iti abhidhammatthasaṅgahe cittasaṅgahavibhāgo nāma",
  "tika-abh07t0:77_0": "Paṭhamo paricchedo."
}