{
  "thag6.8:0.1_0": "Theragāthā",
  "thag6.8:0.2_0": "Chakkanipāta",
  "thag6.8:0.3_0": "Paṭhamavagga",
  "thag6.8:0.4_0": "6.8. Migajālattheragāthā",
  "thag6.8:1.1_0": "“Sudesito cakkhumatā,",
  "thag6.8:1.2_0": "buddhenādiccabandhunā;",
  "thag6.8:1.3_0": "Sabbasaṁyojanātīto,",
  "thag6.8:1.4_0": "sabbavaṭṭavināsano.",
  "thag6.8:2.1_0": "Niyyāniko uttaraṇo,",
  "thag6.8:2.2_0": "taṇhāmūlavisosano;",
  "thag6.8:2.3_0": "Visamūlaṁ āghātanaṁ,",
  "thag6.8:2.4_0": "chetvā pāpeti nibbutiṁ.",
  "thag6.8:3.1_0": "Aññāṇamūlabhedāya,",
  "thag6.8:3.2_0": "kammayantavighāṭano;",
  "thag6.8:3.3_0": "Viññāṇānaṁ pariggahe,",
  "thag6.8:3.4_0": "ñāṇavajiranipātano.",
  "thag6.8:4.1_0": "Vedanānaṁ viññāpano,",
  "thag6.8:4.2_0": "upādānappamocano;",
  "thag6.8:4.3_0": "Bhavaṁ aṅgārakāsuṁva,",
  "thag6.8:4.4_0": "ñāṇena anupassano.",
  "thag6.8:5.1_0": "Mahāraso sugambhīro,",
  "thag6.8:5.2_0": "jarāmaccunivāraṇo;",
  "thag6.8:5.3_0": "Ariyo aṭṭhaṅgiko maggo,",
  "thag6.8:5.4_0": "dukkhūpasamano sivo.",
  "thag6.8:6.1_0": "Kammaṁ kammanti ñatvāna,",
  "thag6.8:6.2_0": "vipākañca vipākato;",
  "thag6.8:6.3_0": "Paṭiccuppannadhammānaṁ,",
  "thag6.8:6.4_0": "yathāvālokadassano;",
  "thag6.8:6.5_0": "Mahākhemaṅgamo santo,",
  "thag6.8:6.6_0": "pariyosānabhaddako”ti.",
  "thag6.8:6.7_0": "… Migajālo thero …."
}