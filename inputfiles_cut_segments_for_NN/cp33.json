{
  "cp33:0.1_0": "Cariyāpiṭaka",
  "cp33:0.2_0": "Yudhañjayavagga",
  "cp33:0.3_0": "Mettāpāramī 1",
  "cp33:0.4_0": "Suvaṇṇasāmacariya",
  "cp33:1.1_0": "“Sāmo yadā vane āsiṁ,",
  "cp33:1.2_0": "sakkena abhinimmito;",
  "cp33:1.3_0": "Pavane sīhabyagghe ca,",
  "cp33:1.4_0": "mettāya mupanāmayiṁ.",
  "cp33:2.1_0": "Sīhabyagghehi dīpīhi,",
  "cp33:2.2_0": "acchehi mahisehi ca;",
  "cp33:2.3_0": "Pasadamigavarāhehi,",
  "cp33:2.4_0": "parivāretvā vane vasiṁ.",
  "cp33:3.1_0": "Na maṁ koci uttasati,",
  "cp33:3.2_0": "napi bhāyāmi kassaci;",
  "cp33:3.3_0": "Mettābalenupatthaddho,",
  "cp33:3.4_0": "ramāmi pavane tadā”ti.",
  "cp33:4.1_0": "Suvaṇṇasāmacariyaṁ terasamaṁ."
}