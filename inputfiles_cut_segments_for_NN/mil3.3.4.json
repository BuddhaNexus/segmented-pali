{
  "mil3.3.4:0.1_0": "Milindapañha",
  "mil3.3.4:0.2_0": "Vicāravagga",
  "mil3.3.4:0.3_0": "4. Saṅkhārajāyamānapañha",
  "mil3.3.4:1.1_0": "Rājā āha—",
  "mil3.3.4:1.2_0": "“bhante nāgasena, atthi keci saṅkhārā, ye jāyantī”ti?",
  "mil3.3.4:1.3_0": "“Āma, mahārāja, atthi saṅkhārā, ye jāyantī”ti.",
  "mil3.3.4:1.4_0": "“Katame te, bhante”ti?",
  "mil3.3.4:1.5_0": "“Cakkhusmiñca kho, mahārāja,",
  "mil3.3.4:1.5_1": "sati rūpesu ca cakkhuviññāṇaṁ hoti,",
  "mil3.3.4:1.5_2": "cakkhuviññāṇe sati cakkhusamphasso hoti,",
  "mil3.3.4:1.5_3": "cakkhusamphasse sati vedanā hoti,",
  "mil3.3.4:1.5_4": "vedanāya sati taṇhā hoti,",
  "mil3.3.4:1.5_5": "taṇhāya sati upādānaṁ hoti,",
  "mil3.3.4:1.5_6": "upādāne sati bhavo hoti,",
  "mil3.3.4:1.5_7": "bhave sati jāti hoti,",
  "mil3.3.4:1.5_8": "jātiyā sati jarāmaraṇaṁ sokaparidevadukkhadomanassupāyāsā sambhavanti,",
  "mil3.3.4:1.5_9": "evametassa kevalassa dukkhakkhandhassa samudayo hoti.",
  "mil3.3.4:1.6_0": "Cakkhusmiñca kho, mahārāja,",
  "mil3.3.4:1.6_1": "asati rūpesu ca asati cakkhuviññāṇaṁ na hoti,",
  "mil3.3.4:1.6_2": "cakkhuviññāṇe asati cakkhusamphasso na hoti,",
  "mil3.3.4:1.6_3": "cakkhusamphasse asati vedanā na hoti,",
  "mil3.3.4:1.6_4": "vedanāya asati taṇhā na hoti,",
  "mil3.3.4:1.6_5": "taṇhāya asati upādānaṁ na hoti,",
  "mil3.3.4:1.6_6": "upādāne asati bhavo na hoti,",
  "mil3.3.4:1.6_7": "bhave asati jāti na hoti,",
  "mil3.3.4:1.6_8": "jātiyā asati jarāmaraṇaṁ sokaparidevadukkhadomanassupāyāsā na honti,",
  "mil3.3.4:1.6_9": "evametassa kevalassa dukkhakkhandhassa nirodho hotī”ti.",
  "mil3.3.4:2.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.3.4:3.1_0": "Saṅkhārajāyamānapañho catuttho."
}