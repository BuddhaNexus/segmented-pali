{
  "tha-ap439:0.1_0": "Therāpadāna",
  "tha-ap439:0.2_0": "Ekavihārivagga",
  "tha-ap439:0.3_0": "7. Ambāṭakadāyakattheraapadāna",
  "tha-ap439:1.1_0": "“Vipine buddhaṁ disvāna,",
  "tha-ap439:1.2_0": "sayambhuṁ aparājitaṁ;",
  "tha-ap439:1.3_0": "Ambāṭakaṁ gahetvāna,",
  "tha-ap439:1.4_0": "sayambhussa adāsahaṁ.",
  "tha-ap439:2.1_0": "Ekatiṁse ito kappe,",
  "tha-ap439:2.2_0": "yaṁ phalamadadiṁ tadā;",
  "tha-ap439:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap439:2.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap439:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap439:3.2_0": "…pe…",
  "tha-ap439:3.3_0": "viharāmi anāsavo.",
  "tha-ap439:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap439:4.2_0": "…pe…",
  "tha-ap439:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap439:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap439:5.2_0": "…pe…",
  "tha-ap439:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap439:6.1_0": "Itthaṁ sudaṁ āyasmā ambāṭakadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap439:7.1_0": "Ambāṭakadāyakattherassāpadānaṁ sattamaṁ."
}