{
  "ps1.10:0.1_0": "Paṭisambhidāmagga",
  "ps1.10:0.2_0": "1 Mahāvagga",
  "ps1.10:0.3_0": "1.10. Maṇḍapeyyakathā",
  "ps1.10:1.1_0": "“Maṇḍapeyyamidaṁ, bhikkhave, brahmacariyaṁ.",
  "ps1.10:1.2_0": "Satthā sammukhībhūto tidhattamaṇḍo.",
  "ps1.10:1.3_0": "Satthari sammukhībhūte desanāmaṇḍo, paṭiggahamaṇḍo, brahmacariyamaṇḍo”.",
  "ps1.10:2.1_0": "Katamo **desanāmaṇḍo?**",
  "ps1.10:2.2_0": "Catunnaṁ ariyasaccānaṁ ācikkhanā desanā paññāpanā paṭṭhapanā vivaraṇā vibhajanā uttānīkammaṁ, catunnaṁ satipaṭṭhānānaṁ …pe…",
  "ps1.10:2.3_0": "catunnaṁ sammappadhānānaṁ …",
  "ps1.10:2.4_0": "catunnaṁ iddhipādānaṁ …",
  "ps1.10:2.5_0": "pañcannaṁ indriyānaṁ …",
  "ps1.10:2.6_0": "pañcannaṁ balānaṁ …",
  "ps1.10:2.7_0": "sattannaṁ bojjhaṅgānaṁ …",
  "ps1.10:2.8_0": "ariyassa aṭṭhaṅgikassa maggassa ācikkhanā desanā paññāpanā paṭṭhapanā vivaraṇā vibhajanā uttānīkammaṁ—",
  "ps1.10:2.9_0": "ayaṁ desanāmaṇḍo. ",
  "ps1.10:3.1_0": "Katamo **paṭiggahamaṇḍo?**",
  "ps1.10:3.2_0": "Bhikkhū bhikkhuniyo upāsakā upāsikāyo devā manussā ye vā panaññepi keci viññātāro—",
  "ps1.10:3.3_0": "ayaṁ paṭiggahamaṇḍo. ",
  "ps1.10:4.1_0": "Katamo **brahmacariyamaṇḍo?**",
  "ps1.10:4.2_0": "Ayameva ariyo aṭṭhaṅgiko maggo, seyyathidaṁ—",
  "ps1.10:4.3_0": "sammādiṭṭhi, sammāsaṅkappo, sammāvācā, sammākammanto, sammāājīvo, sammāvāyāmo, sammāsati, sammāsamādhi—",
  "ps1.10:4.4_0": "ayaṁ brahmacariyamaṇḍo. ",
  "ps1.10:5.1_0": "Adhimokkhamaṇḍo saddhindriyaṁ, assaddhiyaṁ kasaṭo;",
  "ps1.10:5.2_0": "assaddhiyaṁ kasaṭaṁ chaḍḍetvā saddhindriyassa adhimokkhamaṇḍaṁ pivatīti—",
  "ps1.10:5.3_0": "maṇḍapeyyaṁ.",
  "ps1.10:5.4_0": "Paggahamaṇḍo vīriyindriyaṁ, kosajjaṁ kasaṭo;",
  "ps1.10:5.5_0": "kosajjaṁ kasaṭaṁ chaḍḍetvā vīriyindriyassa paggahamaṇḍaṁ pivatīti—",
  "ps1.10:5.6_0": "maṇḍapeyyaṁ.",
  "ps1.10:5.7_0": "Upaṭṭhānamaṇḍo satindriyaṁ, pamādo kasaṭo;",
  "ps1.10:5.8_0": "pamādaṁ kasaṭaṁ chaḍḍetvā satindriyassa upaṭṭhānamaṇḍaṁ pivatīti—",
  "ps1.10:5.9_0": "maṇḍapeyyaṁ.",
  "ps1.10:5.10_0": "Avikkhepamaṇḍo samādhindriyaṁ, uddhaccaṁ kasaṭo;",
  "ps1.10:5.11_0": "uddhaccaṁ kasaṭaṁ chaḍḍetvā samādhindriyassa avikkhepamaṇḍaṁ pivatīti—",
  "ps1.10:5.12_0": "maṇḍapeyyaṁ.",
  "ps1.10:5.13_0": "Dassanamaṇḍo paññindriyaṁ, avijjā kasaṭo;",
  "ps1.10:5.14_0": "avijjaṁ kasaṭaṁ chaḍḍetvā paññindriyassa dassanamaṇḍaṁ pivatīti—",
  "ps1.10:5.15_0": "maṇḍapeyyaṁ.",
  "ps1.10:6.1_0": "Assaddhiye akampiyamaṇḍo saddhābalaṁ, assaddhiyaṁ kasaṭo;",
  "ps1.10:6.2_0": "assaddhiyaṁ kasaṭaṁ chaḍḍetvā saddhābalassa assaddhiye akampiyamaṇḍaṁ pivatīti—",
  "ps1.10:6.3_0": "maṇḍapeyyaṁ.",
  "ps1.10:6.4_0": "Kosajje akampiyamaṇḍo vīriyabalaṁ, kosajjaṁ kasaṭo;",
  "ps1.10:6.5_0": "kosajjaṁ kasaṭaṁ chaḍḍetvā vīriyabalassa kosajje akampiyamaṇḍaṁ pivatīti—",
  "ps1.10:6.6_0": "maṇḍapeyyaṁ.",
  "ps1.10:6.7_0": "Pamāde akampiyamaṇḍo satibalaṁ, pamādo kasaṭo;",
  "ps1.10:6.8_0": "pamādaṁ kasaṭaṁ chaḍḍetvā satibalassa pamāde akampiyamaṇḍaṁ pivatīti—",
  "ps1.10:6.9_0": "maṇḍapeyyaṁ.",
  "ps1.10:6.10_0": "Uddhacce akampiyamaṇḍo samādhibalaṁ, uddhaccaṁ kasaṭo;",
  "ps1.10:6.11_0": "uddhaccaṁ kasaṭaṁ chaḍḍetvā samādhibalassa uddhacce akampiyamaṇḍaṁ pivatīti—",
  "ps1.10:6.12_0": "maṇḍapeyyaṁ.",
  "ps1.10:6.13_0": "Avijjāya akampiyamaṇḍo paññābalaṁ, avijjā kasaṭo;",
  "ps1.10:6.14_0": "avijjaṁ kasaṭaṁ chaḍḍetvā paññābalassa avijjāya akampiyamaṇḍaṁ pivatīti—",
  "ps1.10:6.15_0": "maṇḍapeyyaṁ.",
  "ps1.10:7.1_0": "Upaṭṭhānamaṇḍo satisambojjhaṅgā, pamādo kasaṭo;",
  "ps1.10:7.2_0": "pamādaṁ kasaṭaṁ chaḍḍetvā satisambojjhaṅgassa upaṭṭhānamaṇḍaṁ pivatīti—",
  "ps1.10:7.3_0": "maṇḍapeyyaṁ.",
  "ps1.10:7.4_0": "Pavicayamaṇḍo dhammavicayasambojjhaṅgo, avijjā kasaṭo;",
  "ps1.10:7.5_0": "avijjaṁ kasaṭaṁ chaḍḍetvā dhammavicayasambojjhaṅgassa pavicayamaṇḍaṁ pivatīti—",
  "ps1.10:7.6_0": "maṇḍapeyyaṁ.",
  "ps1.10:7.7_0": "Paggahamaṇḍo vīriyasambojjhaṅgo, kosajjaṁ kasaṭo;",
  "ps1.10:7.8_0": "kosajjaṁ kasaṭaṁ chaḍḍetvā vīriyasambojjhaṅgassa paggahamaṇḍaṁ pivatīti—",
  "ps1.10:7.9_0": "maṇḍapeyyaṁ.",
  "ps1.10:7.10_0": "Pharaṇamaṇḍo pītisambojjhaṅgo, pariḷāho kasaṭo;",
  "ps1.10:7.11_0": "pariḷāhaṁ kasaṭaṁ chaḍḍetvā pītisambojjhaṅgassa pharaṇamaṇḍaṁ pivatīti—",
  "ps1.10:7.12_0": "maṇḍapeyyaṁ.",
  "ps1.10:7.13_0": "Upasamamaṇḍo passaddhisambojjhaṅgo, duṭṭhullaṁ kasaṭo;",
  "ps1.10:7.14_0": "duṭṭhullaṁ kasaṭaṁ chaḍḍetvā passaddhisambojjhaṅgassa upasamamaṇḍaṁ pivatīti—",
  "ps1.10:7.15_0": "maṇḍapeyyaṁ.",
  "ps1.10:7.16_0": "Avikkhepamaṇḍo samādhisambojjhaṅgo, uddhaccaṁ kasaṭo;",
  "ps1.10:7.17_0": "uddhaccaṁ kasaṭaṁ chaḍḍetvā samādhisambojjhaṅgassa avikkhepamaṇḍaṁ pivatīti—",
  "ps1.10:7.18_0": "maṇḍapeyyaṁ.",
  "ps1.10:7.19_0": "Paṭisaṅkhānamaṇḍo upekkhāsambojjhaṅgo, appaṭisaṅkhā kasaṭo;",
  "ps1.10:7.20_0": "appaṭisaṅkhaṁ kasaṭaṁ chaḍḍetvā upekkhāsambojjhaṅgassa paṭisaṅkhānamaṇḍaṁ pivatīti—",
  "ps1.10:7.21_0": "maṇḍapeyyaṁ.",
  "ps1.10:8.1_0": "Dassanamaṇḍo sammādiṭṭhi, micchādiṭṭhi kasaṭo;",
  "ps1.10:8.2_0": "micchādiṭṭhiṁ kasaṭaṁ chaḍḍetvā sammādiṭṭhiyā dassanamaṇḍaṁ pivatīti—",
  "ps1.10:8.3_0": "maṇḍapeyyaṁ.",
  "ps1.10:8.4_0": "Abhiniropanamaṇḍo sammāsaṅkappo, micchāsaṅkappo kasaṭo;",
  "ps1.10:8.5_0": "micchāsaṅkappaṁ kasaṭaṁ chaḍḍetvā sammāsaṅkappassa abhiniropanamaṇḍaṁ pivatīti—",
  "ps1.10:8.6_0": "maṇḍapeyyaṁ.",
  "ps1.10:8.7_0": "Pariggahamaṇḍo sammāvācā, micchāvācā kasaṭo;",
  "ps1.10:8.8_0": "micchāvācaṁ kasaṭaṁ chaḍḍetvā sammāvācāya pariggahamaṇḍaṁ pivatīti—",
  "ps1.10:8.9_0": "maṇḍapeyyaṁ.",
  "ps1.10:8.10_0": "Samuṭṭhānamaṇḍo sammākammanto, micchākammanto kasaṭo;",
  "ps1.10:8.11_0": "micchākammantaṁ kasaṭaṁ chaḍḍetvā sammākammantassa samuṭṭhānamaṇḍaṁ pivatīti—",
  "ps1.10:8.12_0": "maṇḍapeyyaṁ.",
  "ps1.10:8.13_0": "Vodānamaṇḍo sammāājīvo, micchāājīvo kasaṭo;",
  "ps1.10:8.14_0": "micchāājīvaṁ kasaṭaṁ chaḍḍetvā sammāājīvassa vodānamaṇḍaṁ pivatīti—",
  "ps1.10:8.15_0": "maṇḍapeyyaṁ.",
  "ps1.10:8.16_0": "Paggahamaṇḍo sammāvāyāmo, micchāvāyāmo kasaṭo;",
  "ps1.10:8.17_0": "micchāvāyāmaṁ kasaṭaṁ chaḍḍetvā sammāvāyāmassa paggahamaṇḍaṁ pivatīti—",
  "ps1.10:8.18_0": "maṇḍapeyyaṁ.",
  "ps1.10:8.19_0": "Upaṭṭhānamaṇḍo sammāsati, micchāsati kasaṭo;",
  "ps1.10:8.20_0": "micchāsatiṁ kasaṭaṁ chaḍḍetvā sammāsatiyā upaṭṭhānamaṇḍaṁ pivatīti—",
  "ps1.10:8.21_0": "maṇḍapeyyaṁ.",
  "ps1.10:8.22_0": "Avikkhepamaṇḍo sammāsamādhi, micchāsamādhi kasaṭo;",
  "ps1.10:8.23_0": "micchāsamādhiṁ kasaṭaṁ chaḍḍetvā sammāsamādhissa avikkhepamaṇḍaṁ pivatīti—",
  "ps1.10:8.24_0": "maṇḍapeyyaṁ.",
  "ps1.10:9.1_0": "Atthi maṇḍo, atthi peyyaṁ, atthi kasaṭo.",
  "ps1.10:9.2_0": "Adhimokkhamaṇḍo saddhindriyaṁ, assaddhiyaṁ kasaṭo;",
  "ps1.10:9.3_0": "yo tattha attharaso dhammaraso vimuttiraso—",
  "ps1.10:9.4_0": "idaṁ peyyaṁ.",
  "ps1.10:9.5_0": "Paggahamaṇḍo vīriyindriyaṁ, kosajjaṁ kasaṭo;",
  "ps1.10:9.6_0": "yo tattha attharaso dhammaraso vimuttiraso—",
  "ps1.10:9.7_0": "idaṁ peyyaṁ.",
  "ps1.10:9.8_0": "Upaṭṭhānamaṇḍo satindriyaṁ, pamādo kasaṭo;",
  "ps1.10:9.9_0": "yo tattha attharaso dhammaraso vimuttiraso—",
  "ps1.10:9.10_0": "idaṁ peyyaṁ.",
  "ps1.10:9.11_0": "Avikkhepamaṇḍo samādhindriyaṁ, uddhaccaṁ kasaṭo;",
  "ps1.10:9.12_0": "yo tattha attharaso dhammaraso vimuttiraso—",
  "ps1.10:9.13_0": "idaṁ peyyaṁ.",
  "ps1.10:9.14_0": "Dassanamaṇḍo paññindriyaṁ, avijjā kasaṭo;",
  "ps1.10:9.15_0": "yo tattha attharaso dhammaraso vimuttiraso—",
  "ps1.10:9.16_0": "idaṁ peyyaṁ.",
  "ps1.10:10.1_0": "Assaddhiye akampiyamaṇḍo saddhābalaṁ, assaddhiyaṁ kasaṭo;",
  "ps1.10:10.2_0": "yo tattha attharaso dhammaraso vimuttiraso—",
  "ps1.10:10.3_0": "idaṁ peyyaṁ.",
  "ps1.10:10.4_0": "Kosajje akampiyamaṇḍo vīriyabalaṁ, kosajjaṁ kasaṭo;",
  "ps1.10:10.5_0": "yo tattha attharaso dhammaraso vimuttiraso—",
  "ps1.10:10.6_0": "idaṁ peyyaṁ.",
  "ps1.10:10.7_0": "Pamāde akampiyamaṇḍo satibalaṁ, pamādo kasaṭo;",
  "ps1.10:10.8_0": "yo tattha attharaso dhammaraso vimuttiraso—",
  "ps1.10:10.9_0": "idaṁ peyyaṁ.",
  "ps1.10:10.10_0": "Uddhacce akampiyamaṇḍo samādhibalaṁ, uddhaccaṁ kasaṭo;",
  "ps1.10:10.11_0": "yo tattha attharaso dhammaraso vimuttiraso—",
  "ps1.10:10.12_0": "idaṁ peyyaṁ.",
  "ps1.10:10.13_0": "Avijjāya akampiyamaṇḍo paññābalaṁ, avijjā kasaṭo;",
  "ps1.10:10.14_0": "yo tattha attharaso dhammaraso vimuttiraso—",
  "ps1.10:10.15_0": "idaṁ peyyaṁ.",
  "ps1.10:11.1_0": "Upaṭṭhānamaṇḍo satisambojjhaṅgo, pamādo kasaṭo;",
  "ps1.10:11.2_0": "yo tattha attharaso dhammaraso vimuttiraso—",
  "ps1.10:11.3_0": "idaṁ peyyaṁ.",
  "ps1.10:11.4_0": "Pavicayamaṇḍo dhammavicayasambojjhaṅgo, avijjā kasaṭo;",
  "ps1.10:11.5_0": "yo tattha attharaso dhammaraso vimuttiraso—",
  "ps1.10:11.6_0": "idaṁ peyyaṁ.",
  "ps1.10:11.7_0": "Paggahamaṇḍo vīriyasambojjhaṅgo, kosajjaṁ kasaṭo;",
  "ps1.10:11.8_0": "yo tattha attharaso dhammaraso vimuttiraso—",
  "ps1.10:11.9_0": "idaṁ peyyaṁ.",
  "ps1.10:11.10_0": "Pharaṇamaṇḍo pītisambojjhaṅgo, pariḷāho kasaṭo;",
  "ps1.10:11.11_0": "yo tattha attharaso dhammaraso vimuttiraso—",
  "ps1.10:11.12_0": "idaṁ peyyaṁ.",
  "ps1.10:11.13_0": "Upasamamaṇḍo passaddhisambojjhaṅgo, duṭṭhullaṁ kasaṭo;",
  "ps1.10:11.14_0": "yo tattha attharaso dhammaraso vimuttiraso—",
  "ps1.10:11.15_0": "idaṁ peyyaṁ.",
  "ps1.10:11.16_0": "Avikkhepamaṇḍo samādhisambojjhaṅgo, uddhaccaṁ kasaṭo;",
  "ps1.10:11.17_0": "yo tattha attharaso dhammaraso vimuttiraso—",
  "ps1.10:11.18_0": "idaṁ peyyaṁ.",
  "ps1.10:11.19_0": "Paṭisaṅkhānamaṇḍo upekkhāsambojjhaṅgo, apaṭisaṅkhā kasaṭo;",
  "ps1.10:11.20_0": "yo tattha attharaso dhammaraso vimuttiraso—",
  "ps1.10:11.21_0": "idaṁ peyyaṁ.",
  "ps1.10:12.1_0": "Dassanamaṇḍo sammādiṭṭhi, micchādiṭṭhi kasaṭo;",
  "ps1.10:12.2_0": "yo tattha attharaso dhammaraso vimuttiraso—",
  "ps1.10:12.3_0": "idaṁ peyyaṁ.",
  "ps1.10:12.4_0": "Abhiniropanamaṇḍo sammāsaṅkappo, micchāsaṅkappo kasaṭo;",
  "ps1.10:12.5_0": "yo tattha attharaso dhammaraso vimuttiraso—",
  "ps1.10:12.6_0": "idaṁ peyyaṁ.",
  "ps1.10:12.7_0": "Pariggahamaṇḍo sammāvācā, micchāvācā kasaṭo;",
  "ps1.10:12.8_0": "yo tattha attharaso dhammaraso vimuttiraso—",
  "ps1.10:12.9_0": "idaṁ peyyaṁ.",
  "ps1.10:12.10_0": "Samuṭṭhānamaṇḍo sammākammanto, micchākammanto kasaṭo;",
  "ps1.10:12.11_0": "yo tattha attharaso dhammaraso vimuttiraso—",
  "ps1.10:12.12_0": "idaṁ peyyaṁ.",
  "ps1.10:12.13_0": "Vodānamaṇḍo sammāājīvo, micchāājīvo kasaṭo;",
  "ps1.10:12.14_0": "yo tattha attharaso dhammaraso vimuttiraso—",
  "ps1.10:12.15_0": "idaṁ peyyaṁ.",
  "ps1.10:12.16_0": "Paggahamaṇḍo sammāvāyāmo, micchāvāyāmo kasaṭo;",
  "ps1.10:12.17_0": "yo tattha attharaso dhammaraso vimuttiraso—",
  "ps1.10:12.18_0": "idaṁ peyyaṁ.",
  "ps1.10:12.19_0": "Upaṭṭhānamaṇḍo sammāsati, micchāsati kasaṭo;",
  "ps1.10:12.20_0": "yo tattha attharaso dhammaraso vimuttiraso—",
  "ps1.10:12.21_0": "idaṁ peyyaṁ.",
  "ps1.10:12.22_0": "Avikkhepamaṇḍo sammāsamādhi, micchāsamādhi kasaṭo;",
  "ps1.10:12.23_0": "yo tattha attharaso dhammaraso vimuttiraso—",
  "ps1.10:12.24_0": "idaṁ peyyaṁ.",
  "ps1.10:13.1_0": "Dassanamaṇḍo sammādiṭṭhi …",
  "ps1.10:13.2_0": "abhiniropanamaṇḍo sammāsaṅkappo …",
  "ps1.10:13.3_0": "pariggahamaṇḍo sammāvācā …",
  "ps1.10:13.4_0": "samuṭṭhānamaṇḍo sammākammanto …",
  "ps1.10:13.5_0": "vodānamaṇḍo sammāājīvo …",
  "ps1.10:13.6_0": "paggahamaṇḍo sammāvāyāmo …",
  "ps1.10:13.7_0": "upaṭṭhānamaṇḍo sammāsati …",
  "ps1.10:13.8_0": "avikkhepamaṇḍo sammāsamādhi.",
  "ps1.10:14.1_0": "Upaṭṭhānamaṇḍo satisambojjhaṅgo …",
  "ps1.10:14.2_0": "pavicayamaṇḍo dhammavicayasambojjhaṅgo …",
  "ps1.10:14.3_0": "paggahamaṇḍo vīriyasambojjhaṅgo …",
  "ps1.10:14.4_0": "pharaṇamaṇḍo pītisambojjhaṅgo …",
  "ps1.10:14.5_0": "upasamamaṇḍo passaddhisambojjhaṅgo …",
  "ps1.10:14.6_0": "avikkhepamaṇḍo samādhisambojjhaṅgo …",
  "ps1.10:14.7_0": "paṭisaṅkhānamaṇḍo upekkhāsambojjhaṅgo.",
  "ps1.10:15.1_0": "Assaddhiye akampiyamaṇḍo saddhābalaṁ …",
  "ps1.10:15.2_0": "kosajje akampiyamaṇḍo vīriyabalaṁ …",
  "ps1.10:15.3_0": "pamāde akampiyamaṇḍo satibalaṁ …",
  "ps1.10:15.4_0": "uddhacce akampiyamaṇḍo samādhibalaṁ …",
  "ps1.10:15.5_0": "avijjāya akampiyamaṇḍo paññābalaṁ.",
  "ps1.10:16.1_0": "Adhimokkhamaṇḍo saddhindriyaṁ …",
  "ps1.10:16.2_0": "paggahamaṇḍo vīriyindriyaṁ …",
  "ps1.10:16.3_0": "upaṭṭhānamaṇḍo satindriyaṁ …",
  "ps1.10:16.4_0": "avikkhepamaṇḍo samādhindriyaṁ …",
  "ps1.10:16.5_0": "dassanamaṇḍo paññindriyaṁ.",
  "ps1.10:17.1_0": "Ādhipateyyaṭṭhena indriyā maṇḍo, akampiyaṭṭhena balā maṇḍo,",
  "ps1.10:17.1_1": "niyyānaṭṭhena bojjhaṅgā maṇḍo, hetuṭṭhena maggo maṇḍo,",
  "ps1.10:17.1_2": "upaṭṭhānaṭṭhena satipaṭṭhānā maṇḍo,",
  "ps1.10:17.1_3": "padahanaṭṭhena sammappadhānā maṇḍo,",
  "ps1.10:17.1_4": "ijjhanaṭṭhena iddhipādā maṇḍo, tathaṭṭhena saccā maṇḍo,",
  "ps1.10:17.1_5": "avikkhepaṭṭhena samatho maṇḍo,",
  "ps1.10:17.1_6": "anupassanaṭṭhena vipassanā maṇḍo,",
  "ps1.10:17.1_7": "ekarasaṭṭhena samathavipassanā maṇḍo,",
  "ps1.10:17.1_8": "anativattanaṭṭhena yuganaddhā maṇḍo,",
  "ps1.10:17.1_9": "saṁvaraṭṭhena sīlavisuddhi maṇḍo,",
  "ps1.10:17.1_10": "avikkhepaṭṭhena cittavisuddhi maṇḍo,",
  "ps1.10:17.1_11": "dassanaṭṭhena diṭṭhivisuddhi maṇḍo, muttaṭṭhena vimokkho maṇḍo, paṭivedhaṭṭhena vijjā maṇḍo,",
  "ps1.10:17.1_12": "pariccāgaṭṭhena vimutti maṇḍo,",
  "ps1.10:17.1_13": "samucchedaṭṭhena khaye ñāṇaṁ maṇḍo,",
  "ps1.10:17.1_14": "paṭippassaddhaṭṭhena anuppāde ñāṇaṁ maṇḍo, chando mūlaṭṭhena maṇḍo,",
  "ps1.10:17.1_15": "manasikāro samuṭṭhānaṭṭhena maṇḍo,",
  "ps1.10:17.1_16": "phasso samodhānaṭṭhena maṇḍo,",
  "ps1.10:17.1_17": "vedanā samosaraṇaṭṭhena maṇḍo, samādhi pamukhaṭṭhena maṇḍo,",
  "ps1.10:17.1_18": "sati ādhipateyyaṭṭhena maṇḍo, paññā taduttaraṭṭhena maṇḍo, vimutti sāraṭṭhena maṇḍo,",
  "ps1.10:17.1_19": "amatogadhaṁ nibbānaṁ pariyosānaṭṭhena maṇḍoti.",
  "ps1.10:17.2_0": "Maṇḍapeyyakathā niṭṭhitā.",
  "ps1.10:17.3_0": "Catutthabhāṇavāro.",
  "ps1.10:17.4_0": "Mahāvaggo paṭhamo.",
  "ps1.10:17.5_0": "Tassuddānaṁ",
  "ps1.10:18.1_0": "Ñāṇadiṭṭhī ca assāsā,",
  "ps1.10:18.2_0": "indriyaṁ vimokkhapañcamā;",
  "ps1.10:18.3_0": "Gatikammavipallāsā,",
  "ps1.10:18.4_0": "maggo maṇḍena te dasāti.",
  "ps1.10:18.5_0": "Esa nikāyadharehi ṭhapito,",
  "ps1.10:18.5_1": "asamo paṭhamo pavaro varavaggoti."
}