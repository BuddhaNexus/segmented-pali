{
  "tha-ap406:0.1_0": "Therāpadāna",
  "tha-ap406:0.2_0": "Metteyyavagga",
  "tha-ap406:0.3_0": "4. Dhotakattheraapadāna",
  "tha-ap406:1.1_0": "“Gaṅgā bhāgīrathī nāma,",
  "tha-ap406:1.2_0": "himavantā pabhāvitā;",
  "tha-ap406:1.3_0": "Haṁsavatiyā dvārena,",
  "tha-ap406:1.4_0": "anusandati tāvade.",
  "tha-ap406:2.1_0": "Sobhito nāma ārāmo,",
  "tha-ap406:2.2_0": "gaṅgākūle sumāpito;",
  "tha-ap406:2.3_0": "Tattha padumuttaro buddho,",
  "tha-ap406:2.4_0": "vasate lokanāyako.",
  "tha-ap406:3.1_0": "Tidasehi yathā indo,",
  "tha-ap406:3.2_0": "manujehi purakkhato;",
  "tha-ap406:3.3_0": "Nisīdi tattha bhagavā,",
  "tha-ap406:3.4_0": "asambhītova kesarī.",
  "tha-ap406:4.1_0": "Nagare haṁsavatiyā,",
  "tha-ap406:4.2_0": "vasāmi brāhmaṇo ahaṁ;",
  "tha-ap406:4.3_0": "Chaḷaṅgo nāma nāmena,",
  "tha-ap406:4.4_0": "evaṁnāmo mahāmuni.",
  "tha-ap406:5.1_0": "Aṭṭhārasa sissasatā,",
  "tha-ap406:5.2_0": "parivārenti maṁ tadā;",
  "tha-ap406:5.3_0": "Tehi sissehi samito,",
  "tha-ap406:5.4_0": "gaṅgātīraṁ upāgamiṁ.",
  "tha-ap406:6.1_0": "Tatthaddasāsiṁ samaṇe,",
  "tha-ap406:6.2_0": "nikkuhe dhotapāpake;",
  "tha-ap406:6.3_0": "Bhāgīrathiṁ tarantehaṁ,",
  "tha-ap406:6.4_0": "evaṁ cintesi tāvade.",
  "tha-ap406:7.1_0": "‘Sāyaṁ pātaṁ tarantāme,",
  "tha-ap406:7.2_0": "buddhaputtā mahāyasā;",
  "tha-ap406:7.3_0": "Vihesayanti attānaṁ,",
  "tha-ap406:7.4_0": "tesaṁ attā vihaññati.",
  "tha-ap406:8.1_0": "Sadevakassa lokassa,",
  "tha-ap406:8.2_0": "buddho aggo pavuccati;",
  "tha-ap406:8.3_0": "Natthi me dakkhiṇe kāraṁ,",
  "tha-ap406:8.4_0": "gatimaggavisodhanaṁ.",
  "tha-ap406:9.1_0": "Yannūna buddhaseṭṭhassa,",
  "tha-ap406:9.2_0": "setuṁ gaṅgāya kāraye;",
  "tha-ap406:9.3_0": "Kārāpetvā imaṁ kammaṁ,",
  "tha-ap406:9.4_0": "santarāmi imaṁ bhavaṁ’.",
  "tha-ap406:10.1_0": "Satasahassaṁ datvāna,",
  "tha-ap406:10.2_0": "setuṁ kārāpayiṁ ahaṁ;",
  "tha-ap406:10.3_0": "Saddahanto kataṁ kāraṁ,",
  "tha-ap406:10.4_0": "vipulaṁ me bhavissati.",
  "tha-ap406:11.1_0": "Kārāpetvāna taṁ setuṁ,",
  "tha-ap406:11.2_0": "upesiṁ lokanāyakaṁ;",
  "tha-ap406:11.3_0": "Sirasi añjaliṁ katvā,",
  "tha-ap406:11.4_0": "imaṁ vacanamabraviṁ.",
  "tha-ap406:12.1_0": "‘Satasahassassa vayaṁ,",
  "tha-ap406:12.2_0": "datvā kārāpito mayā;",
  "tha-ap406:12.3_0": "Tavatthāya mahāsetu,",
  "tha-ap406:12.4_0": "paṭiggaṇha mahāmune’.",
  "tha-ap406:13.1_0": "Padumuttaro lokavidū,",
  "tha-ap406:13.2_0": "āhutīnaṁ paṭiggaho;",
  "tha-ap406:13.3_0": "Bhikkhusaṅghe nisīditvā,",
  "tha-ap406:13.4_0": "imā gāthā abhāsatha.",
  "tha-ap406:14.1_0": "‘Yo me setuṁ akāresi,",
  "tha-ap406:14.2_0": "pasanno sehi pāṇibhi;",
  "tha-ap406:14.3_0": "Tamahaṁ kittayissāmi,",
  "tha-ap406:14.4_0": "suṇātha mama bhāsato.",
  "tha-ap406:15.0_0": "4.1. Setudānaānisaṁsa",
  "tha-ap406:15.1_0": "Darito pabbatato vā,",
  "tha-ap406:15.2_0": "rukkhato patitopiyaṁ;",
  "tha-ap406:15.3_0": "Cutopi lacchatī ṭhānaṁ,",
  "tha-ap406:15.4_0": "setudānassidaṁ phalaṁ.",
  "tha-ap406:16.1_0": "Virūḷhamūlasantānaṁ,",
  "tha-ap406:16.2_0": "nigrodhamiva māluto;",
  "tha-ap406:16.3_0": "Amittā nappasahanti,",
  "tha-ap406:16.4_0": "setudānassidaṁ phalaṁ.",
  "tha-ap406:17.1_0": "Nāssa corā pasahanti,",
  "tha-ap406:17.2_0": "nātimaññanti khattiyā;",
  "tha-ap406:17.3_0": "Sabbe tarissatāmitte,",
  "tha-ap406:17.4_0": "setudānassidaṁ phalaṁ.",
  "tha-ap406:18.1_0": "Abbhokāsagataṁ santaṁ,",
  "tha-ap406:18.2_0": "kaṭhinātapatāpitaṁ;",
  "tha-ap406:18.3_0": "Puññakammena saṁyuttaṁ,",
  "tha-ap406:18.4_0": "na bhavissati vedanā.",
  "tha-ap406:19.1_0": "Devaloke manusse vā,",
  "tha-ap406:19.2_0": "hatthiyānaṁ sunimmitaṁ;",
  "tha-ap406:19.3_0": "Tassa saṅkappamaññāya,",
  "tha-ap406:19.4_0": "nibbattissati tāvade.",
  "tha-ap406:20.1_0": "Sahassassā vātajavā,",
  "tha-ap406:20.2_0": "sindhavā sīghavāhanā;",
  "tha-ap406:20.3_0": "Sāyaṁ pātaṁ upessanti,",
  "tha-ap406:20.4_0": "setudānassidaṁ phalaṁ.",
  "tha-ap406:21.1_0": "Āgantvāna manussattaṁ,",
  "tha-ap406:21.2_0": "sukhitoyaṁ bhavissati;",
  "tha-ap406:21.3_0": "Vehāsaṁ manujasseva,",
  "tha-ap406:21.4_0": "hatthiyānaṁ bhavissati.",
  "tha-ap406:22.1_0": "Kappasatasahassamhi,",
  "tha-ap406:22.2_0": "okkākakulasambhavo;",
  "tha-ap406:22.3_0": "Gotamo nāma gottena,",
  "tha-ap406:22.4_0": "satthā loke bhavissati.",
  "tha-ap406:23.1_0": "Tassa dhammesu dāyādo,",
  "tha-ap406:23.2_0": "oraso dhammanimmito;",
  "tha-ap406:23.3_0": "Sabbāsave pariññāya,",
  "tha-ap406:23.4_0": "nibbāyissatināsavo’.",
  "tha-ap406:24.1_0": "Aho me sukataṁ kammaṁ,",
  "tha-ap406:24.2_0": "jalajuttamanāmake;",
  "tha-ap406:24.3_0": "Tattha kāraṁ karitvāna,",
  "tha-ap406:24.4_0": "pattohaṁ āsavakkhayaṁ.",
  "tha-ap406:25.1_0": "Padhānaṁ pahitattomhi,",
  "tha-ap406:25.2_0": "upasanto nirūpadhi;",
  "tha-ap406:25.3_0": "Nāgova bandhanaṁ chetvā,",
  "tha-ap406:25.4_0": "viharāmi anāsavo.",
  "tha-ap406:26.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap406:26.2_0": "…pe…",
  "tha-ap406:26.3_0": "viharāmi anāsavo.",
  "tha-ap406:27.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap406:27.2_0": "…pe…",
  "tha-ap406:27.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap406:28.1_0": "Paṭisambhidā catasso,",
  "tha-ap406:28.2_0": "…pe…",
  "tha-ap406:28.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap406:29.1_0": "Itthaṁ sudaṁ āyasmā dhotako thero imā gāthāyo abhāsitthāti.",
  "tha-ap406:30.1_0": "Dhotakattherassāpadānaṁ catutthaṁ."
}