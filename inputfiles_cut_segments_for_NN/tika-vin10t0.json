{
  "tika-vin10t0:0_0": " Namo tassa bhagavato arahato sammāsambuddhassa",
  "tika-vin10t0:1_0": "Vinayavinicchayo",
  "tika-vin10t0:2_0": "Ganthārambhakathā",
  "tika-vin10t0:3_0": "1.",
  "tika-vin10t0:4_0": " Vanditvā sirasā seṭṭhaṁ, buddhamappaṭipuggalaṁ;",
  "tika-vin10t0:5_0": "Bhavābhāvakaraṁ dhammaṁ, gaṇañceva niraṅgaṇaṁ.",
  "tika-vin10t0:6_0": "2.",
  "tika-vin10t0:7_0": " Bhikkhūnaṁ bhikkhunīnañca, hitatthāya samāhito;",
  "tika-vin10t0:8_0": "Pavakkhāmi samāsena, vinayassavinicchayaṁ.",
  "tika-vin10t0:9_0": "3.",
  "tika-vin10t0:10_0": " Anākulamasaṁkiṇṇaṁ, madhuratthapadakkamaṁ;",
  "tika-vin10t0:11_0": "Paṭubhāvakaraṁ etaṁ, paramaṁ vinayakkame.",
  "tika-vin10t0:12_0": "4.",
  "tika-vin10t0:13_0": " Apāraṁ otarantānaṁ, sāraṁ vinayasāgaraṁ;",
  "tika-vin10t0:14_0": "Bhikkhūnaṁ bhikkhunīnañca, nāvābhūtaṁ manoramaṁ.",
  "tika-vin10t0:15_0": "5.",
  "tika-vin10t0:16_0": " Tasmā vinayanūpāyaṁ, vinayassavinicchayaṁ;",
  "tika-vin10t0:17_0": "Avikkhittena cittena, vadato me nibodhatha."
}