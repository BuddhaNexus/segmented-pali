{
  "kv16.1:0.1_0": "Kathāvatthu",
  "kv16.1:0.2_0": "Niggahapaṇṇāsaka",
  "kv16.1:0.3_0": "Soḷasamavagga",
  "kv16.1:0.4_0": "Niggahakathā",
  "kv16.1:1.1_0": "Paro parassa cittaṁ niggaṇhātīti?",
  "kv16.1:1.2_0": "Āmantā.",
  "kv16.1:1.3_0": "Paro parassa cittaṁ “mā rajji”, “mā dussi”, “mā muyhi”, “mā kilissī”ti niggaṇhātīti?",
  "kv16.1:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv16.1:1.5_0": "paro parassa cittaṁ niggaṇhātīti?",
  "kv16.1:1.6_0": "Āmantā.",
  "kv16.1:1.7_0": "Paro parassa uppanno phasso “mā nirujjhī”ti niggaṇhātīti?",
  "kv16.1:1.8_0": "Na hevaṁ vattabbe …pe…",
  "kv16.1:1.9_0": "paro parassa uppannā vedanā …pe…",
  "kv16.1:1.10_0": "uppannā saññā …",
  "kv16.1:1.11_0": "uppannā cetanā …",
  "kv16.1:1.12_0": "uppannaṁ cittaṁ …",
  "kv16.1:1.13_0": "uppannā saddhā …",
  "kv16.1:1.14_0": "uppannaṁ vīriyaṁ …",
  "kv16.1:1.15_0": "uppannā sati …",
  "kv16.1:1.16_0": "uppanno samādhi …pe…",
  "kv16.1:1.17_0": "uppannā paññā “mā nirujjhī”ti niggaṇhātīti?",
  "kv16.1:1.18_0": "Na hevaṁ vattabbe …pe….",
  "kv16.1:2.1_0": "Paro parassa cittaṁ niggaṇhātīti?",
  "kv16.1:2.2_0": "Āmantā.",
  "kv16.1:2.3_0": "Paro parassa atthāya rāgaṁ pajahati …",
  "kv16.1:2.4_0": "dosaṁ pajahati …pe…",
  "kv16.1:2.5_0": "anottappaṁ pajahatīti?",
  "kv16.1:2.6_0": "Na hevaṁ vattabbe …pe….",
  "kv16.1:3.1_0": "Paro parassa cittaṁ niggaṇhātīti?",
  "kv16.1:3.2_0": "Āmantā.",
  "kv16.1:3.3_0": "Paro parassa atthāya maggaṁ bhāveti …",
  "kv16.1:3.4_0": "satipaṭṭhānaṁ bhāveti …pe…",
  "kv16.1:3.5_0": "bojjhaṅgaṁ bhāvetīti?",
  "kv16.1:3.6_0": "Na hevaṁ vattabbe …pe….",
  "kv16.1:4.1_0": "Paro parassa cittaṁ niggaṇhātīti?",
  "kv16.1:4.2_0": "Āmantā.",
  "kv16.1:4.3_0": "Paro parassa atthāya dukkhaṁ parijānāti, samudayaṁ pajahati, nirodhaṁ sacchikaroti, maggaṁ bhāvetīti?",
  "kv16.1:4.4_0": "Na hevaṁ vattabbe …pe….",
  "kv16.1:5.1_0": "Paro parassa cittaṁ niggaṇhātīti?",
  "kv16.1:5.2_0": "Āmantā.",
  "kv16.1:5.3_0": "Añño aññassa kārako,",
  "kv16.1:5.3_1": "paraṅkataṁ sukhaṁ dukkhaṁ añño karoti añño paṭisaṁvedetīti?",
  "kv16.1:5.4_0": "Na hevaṁ vattabbe …pe….",
  "kv16.1:6.1_0": "Paro parassa cittaṁ niggaṇhātīti?",
  "kv16.1:6.2_0": "Āmantā.",
  "kv16.1:6.3_0": "Nanu vuttaṁ bhagavatā—",
  "kv16.1:7.1_0": "“Attanāva kataṁ pāpaṁ,",
  "kv16.1:7.2_0": "attanā saṅkilissati;",
  "kv16.1:7.3_0": "Attanā akataṁ pāpaṁ,",
  "kv16.1:7.4_0": "attanāva visujjhati;",
  "kv16.1:7.5_0": "Suddhi asuddhi paccattaṁ,",
  "kv16.1:7.6_0": "nāñño aññaṁ visodhaye”ti.",
  "kv16.1:8.1_0": "Attheva suttantoti?",
  "kv16.1:8.2_0": "Āmantā.",
  "kv16.1:8.3_0": "Tena hi na vattabbaṁ—",
  "kv16.1:8.4_0": "“paro parassa cittaṁ niggaṇhātī”ti.",
  "kv16.1:9.1_0": "Na vattabbaṁ—",
  "kv16.1:9.2_0": "“paro parassa cittaṁ niggaṇhātī”ti?",
  "kv16.1:9.3_0": "Āmantā.",
  "kv16.1:9.4_0": "Nanu atthi balappattā, atthi vasībhūtāti?",
  "kv16.1:9.5_0": "Āmantā.",
  "kv16.1:9.6_0": "Hañci atthi balappattā, atthi vasībhūtā, tena vata re vattabbe—",
  "kv16.1:9.7_0": "“paro parassa cittaṁ niggaṇhātī”ti.",
  "kv16.1:9.8_0": "Niggahakathā niṭṭhitā."
}