{
  "tha-ap305:0.1_0": "Therāpadāna",
  "tha-ap305:0.2_0": "Padumakesaravagga",
  "tha-ap305:0.3_0": "3. Paramannadāyakattheraapadāna",
  "tha-ap305:1.1_0": "“Kaṇikāraṁva jotantaṁ,",
  "tha-ap305:1.2_0": "udayantaṁva bhāṇumaṁ;",
  "tha-ap305:1.3_0": "Vipassiṁ addasaṁ buddhaṁ,",
  "tha-ap305:1.4_0": "lokajeṭṭhaṁ narāsabhaṁ.",
  "tha-ap305:2.1_0": "Añjaliṁ paggahetvāna,",
  "tha-ap305:2.2_0": "abhinesiṁ sakaṁ gharaṁ;",
  "tha-ap305:2.3_0": "Abhinetvāna sambuddhaṁ,",
  "tha-ap305:2.4_0": "paramannamadāsahaṁ.",
  "tha-ap305:3.1_0": "Ekanavutito kappe,",
  "tha-ap305:3.2_0": "paramannamadiṁ tadā;",
  "tha-ap305:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap305:3.4_0": "paramannassidaṁ phalaṁ.",
  "tha-ap305:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap305:4.2_0": "…pe…",
  "tha-ap305:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap305:5.1_0": "Itthaṁ sudaṁ āyasmā paramannadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap305:6.1_0": "Paramannadāyakattherassāpadānaṁ tatiyaṁ."
}