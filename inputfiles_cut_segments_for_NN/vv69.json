{
  "vv69:0.1_0": "Vimānavatthu",
  "vv69:0.2_0": "Purisavimāna",
  "vv69:0.3_0": "Pāyāsivagga",
  "vv69:0.4_0": "5. Dutiyaupassayadāyakavimānavatthu",
  "vv69:1.1_0": "Suriyo yathā vigatavalāhake nabhe,",
  "vv69:1.2_0": "…pe…",
  "vv69:2.1_0": "(Yathā purimavimānaṁ tathā vitthāretabbaṁ.)",
  "vv69:3.1_0": "“Tena metādiso vaṇṇo,",
  "vv69:3.2_0": "…pe…",
  "vv69:3.3_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv69:4.1_0": "Dutiyaupassayadāyakavimānaṁ pañcamaṁ."
}