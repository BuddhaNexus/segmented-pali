{
  "atk-s0403a32:0_0": "7. Devatāvaggo",
  "atk-s0403a32:1_0": "1-3. Anāgāmiphalasuttādivaṇṇanā",
  "atk-s0403a32:2_0": "65-67.",
  "atk-s0403a32:2_1": "Sattamassa paṭhame assaddhiyanti assaddhabhāvaṁ.",
  "atk-s0403a32:2_2": "Duppaññatanti nippaññabhāvaṁ.",
  "atk-s0403a32:2_3": "Dutiye pamādanti sativippavāsaṁ.",
  "atk-s0403a32:2_4": "Tatiye ābhisamācārikanti uttamasamācārabhūtaṁ vattavasena paṇṇattisīlaṁ.",
  "atk-s0403a32:2_5": "Sekhadhammanti sekhapaṇṇattisīlaṁ.",
  "atk-s0403a32:2_6": "Sīlānīti cattāri mahāsīlāni.",
  "atk-s0403a32:3_0": "4. Saṅgaṇikārāmasuttavaṇṇanā",
  "atk-s0403a32:4_0": "68.",
  "atk-s0403a32:4_1": "Catutthe saṅgaṇikārāmoti gaṇasaṅgaṇikārāmo.",
  "atk-s0403a32:4_2": "Suttantikagaṇādīsu pana gaṇesu attano vā parisāsaṅkhāte gaṇe ramatīti gaṇārāmo.",
  "atk-s0403a32:4_3": "Paviveketi kāyaviveke.",
  "atk-s0403a32:4_4": "Cittassa nimittanti samādhivipassanācittassa nimittaṁ samādhivipassanākāraṁ.",
  "atk-s0403a32:4_5": "Sammādiṭṭhinti vipassanāsammādiṭṭhiṁ.",
  "atk-s0403a32:4_6": "Samādhinti maggasamādhiñceva phalasamādhiñca.",
  "atk-s0403a32:4_7": "Saṁyojanānīti dasa saṁyojanāni.",
  "atk-s0403a32:4_8": "Nibbānanti apaccayaparinibbānaṁ.",
  "atk-s0403a32:5_0": "5. Devatāsuttavaṇṇanā",
  "atk-s0403a32:6_0": "69.",
  "atk-s0403a32:6_1": "Pañcame sovacassatāti subbacabhāvo.",
  "atk-s0403a32:6_2": "Kalyāṇamittatāti sucimittatā.",
  "atk-s0403a32:6_3": "Satthugāravoti satthari gāravayutto.",
  "atk-s0403a32:6_4": "Esa nayo sabbattha.",
  "atk-s0403a32:7_0": "6. Samādhisuttavaṇṇanā",
  "atk-s0403a32:8_0": "70.",
  "atk-s0403a32:8_1": "Chaṭṭhe na santenāti paccanīkakilesehi avūpasantena.",
  "atk-s0403a32:8_2": "Na paṇītenāti na atappakena.",
  "atk-s0403a32:8_3": "Na paṭippassaddhiladdhenāti kilesappaṭippassaddhiyā aladdhena appattena.",
  "atk-s0403a32:8_4": "Na ekodibhāvādhigatenāti na ekaggabhāvaṁ upagatena.",
  "atk-s0403a32:9_0": "7. Sakkhibhabbasuttavaṇṇanā",
  "atk-s0403a32:10_0": "71.",
  "atk-s0403a32:10_1": "Sattame tatra tatrāti tasmiṁ tasmiṁ visese.",
  "atk-s0403a32:10_2": "Sakkhibhabbatanti paccakkhabhāvaṁ.",
  "atk-s0403a32:10_3": "Āyataneti kāraṇe.",
  "atk-s0403a32:10_4": "Hānabhāgiyādayo visuddhimagge saṁvaṇṇitā.",
  "atk-s0403a32:10_5": "Asakkaccakārīti na sukatakārī,",
  "atk-s0403a32:10_6": "na ādarakārī.",
  "atk-s0403a32:10_7": "Asappāyakārīti na sappāyakārī,",
  "atk-s0403a32:10_8": "na upakārabhūtadhammakārī.",
  "atk-s0403a32:11_0": "8. Balasuttavaṇṇanā",
  "atk-s0403a32:12_0": "72.",
  "atk-s0403a32:12_1": "Aṭṭhame balatanti balabhāvaṁ thāmabhāvaṁ.",
  "atk-s0403a32:12_2": "Asātaccakārīti na satatakārī.",
  "atk-s0403a32:12_3": "Sesaṁ heṭṭhā vuttanayameva.",
  "atk-s0403a32:13_0": "9-10. Tajjhānasuttadvayavaṇṇanā",
  "atk-s0403a32:14_0": "73-74.",
  "atk-s0403a32:14_1": "Navame na yathābhūtaṁ sammappaññāya sudiṭṭho hotīti vatthukāmakilesakāmesu ādīnavo na yathāsabhāvato jhānapaññāya sudiṭṭho hoti.",
  "atk-s0403a32:14_2": "Dasamaṁ uttānatthamevāti.",
  "atk-s0403a32:15_0": "Devatāvaggo sattamo."
}