{
  "kv20.2:0.1_0": "Kathāvatthu",
  "kv20.2:0.2_0": "Mahāpaṇṇāsaka",
  "kv20.2:0.3_0": "Vīsatimavagga",
  "kv20.2:0.4_0": "Ñāṇakathā",
  "kv20.2:1.1_0": "Natthi puthujjanassa ñāṇanti?",
  "kv20.2:1.2_0": "Āmantā.",
  "kv20.2:1.3_0": "Natthi puthujjanassa paññā pajānanā vicayo pavicayo dhammavicayo sallakkhaṇā upalakkhaṇā paccupalakkhaṇāti?",
  "kv20.2:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv20.2:1.5_0": "nanu atthi puthujjanassa paññā pajānanā vicayo …pe…",
  "kv20.2:1.6_0": "paccupalakkhaṇāti?",
  "kv20.2:1.7_0": "Āmantā.",
  "kv20.2:1.8_0": "Hañci atthi puthujjanassa paññā pajānanā vicayo …pe…",
  "kv20.2:1.9_0": "paccupalakkhaṇā, no ca vata re vattabbe—",
  "kv20.2:1.10_0": "“natthi puthujjanassa ñāṇan”ti.",
  "kv20.2:2.1_0": "Natthi puthujjanassa ñāṇanti?",
  "kv20.2:2.2_0": "Āmantā.",
  "kv20.2:2.3_0": "Puthujjano paṭhamaṁ jhānaṁ samāpajjeyyāti?",
  "kv20.2:2.4_0": "Āmantā.",
  "kv20.2:2.5_0": "Hañci puthujjano paṭhamaṁ jhānaṁ samāpajjeyya, no ca vata re vattabbe—",
  "kv20.2:2.6_0": "“natthi puthujjanassa ñāṇan”ti.",
  "kv20.2:3.1_0": "Puthujjano dutiyaṁ jhānaṁ …pe…",
  "kv20.2:3.2_0": "tatiyaṁ jhānaṁ …pe…",
  "kv20.2:3.3_0": "catutthaṁ jhānaṁ …pe…",
  "kv20.2:3.4_0": "ākāsānañcāyatanaṁ samāpajjeyya,",
  "kv20.2:3.5_0": "viññāṇañcāyatanaṁ ākiñcaññāyatanaṁ nevasaññānāsaññāyatanaṁ samāpajjeyya,",
  "kv20.2:3.6_0": "puthujjano dānaṁ dadeyya …pe…",
  "kv20.2:3.7_0": "cīvaraṁ dadeyya,",
  "kv20.2:3.8_0": "piṇḍapātaṁ dadeyya,",
  "kv20.2:3.9_0": "senāsanaṁ dadeyya,",
  "kv20.2:3.10_0": "gilānapaccayabhesajjaparikkhāraṁ dadeyyāti?",
  "kv20.2:3.11_0": "Āmantā.",
  "kv20.2:3.12_0": "Hañci puthujjano gilānapaccayabhesajjaparikkhāraṁ dadeyya, no ca vata re vattabbe—",
  "kv20.2:3.13_0": "“natthi puthujjanassa ñāṇan”ti.",
  "kv20.2:4.1_0": "Atthi puthujjanassa ñāṇanti?",
  "kv20.2:4.2_0": "Āmantā.",
  "kv20.2:4.3_0": "Puthujjano tena ñāṇena dukkhaṁ parijānāti,",
  "kv20.2:4.3_1": "samudayaṁ pajahati,",
  "kv20.2:4.3_2": "nirodhaṁ sacchikaroti,",
  "kv20.2:4.3_3": "maggaṁ bhāvetīti?",
  "kv20.2:4.4_0": "Na hevaṁ vattabbe …pe….",
  "kv20.2:4.5_0": "Ñāṇakathā niṭṭhitā."
}