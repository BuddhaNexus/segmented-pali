{
  "tha-ap214:0.1_0": "Therāpadāna",
  "tha-ap214:0.2_0": "Hatthivagga",
  "tha-ap214:0.3_0": "2. Pānadhidāyakattheraapadāna",
  "tha-ap214:1.1_0": "“Āraññikassa isino,",
  "tha-ap214:1.2_0": "cirarattatapassino;",
  "tha-ap214:1.3_0": "Vuddhassa bhāvitattassa,",
  "tha-ap214:1.4_0": "adāsiṁ pānadhiṁ ahaṁ.",
  "tha-ap214:2.1_0": "Tena kammena dvipadinda,",
  "tha-ap214:2.2_0": "lokajeṭṭha narāsabha;",
  "tha-ap214:2.3_0": "Dibbayānaṁ anubhomi,",
  "tha-ap214:2.4_0": "pubbakammassidaṁ phalaṁ.",
  "tha-ap214:3.1_0": "Catunnavute ito kappe,",
  "tha-ap214:3.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap214:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap214:3.4_0": "pānadhissa idaṁ phalaṁ.",
  "tha-ap214:4.1_0": "Sattasattatito kappe,",
  "tha-ap214:4.2_0": "aṭṭha āsiṁsu khattiyā;",
  "tha-ap214:4.3_0": "Suyānā nāma nāmena,",
  "tha-ap214:4.4_0": "cakkavattī mahabbalā.",
  "tha-ap214:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap214:5.2_0": "…pe…",
  "tha-ap214:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap214:6.1_0": "Itthaṁ sudaṁ āyasmā pānadhidāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap214:7.1_0": "Pānadhidāyakattherassāpadānaṁ dutiyaṁ."
}