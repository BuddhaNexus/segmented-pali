{
  "tha-ap407:0.1_0": "Therāpadāna",
  "tha-ap407:0.2_0": "Metteyyavagga",
  "tha-ap407:0.3_0": "5 Upasivattheraapadāna",
  "tha-ap407:1.1_0": "“Himavantassāvidūre,",
  "tha-ap407:1.2_0": "anomo nāma pabbato;",
  "tha-ap407:1.3_0": "Assamo sukato mayhaṁ,",
  "tha-ap407:1.4_0": "paṇṇasālā sumāpitā.",
  "tha-ap407:2.1_0": "Nadī ca sandatī tattha,",
  "tha-ap407:2.2_0": "supatitthā manoramā;",
  "tha-ap407:2.3_0": "Anūpatitthe jāyanti,",
  "tha-ap407:2.4_0": "padumuppalakā bahū.",
  "tha-ap407:3.1_0": "Pāṭhīnā pāvusā macchā,",
  "tha-ap407:3.2_0": "balajā muñjarohitā;",
  "tha-ap407:3.3_0": "Macchakacchapasañchannā,",
  "tha-ap407:3.4_0": "nadikā sandate tadā.",
  "tha-ap407:4.1_0": "Timirā pupphitā tattha,",
  "tha-ap407:4.2_0": "asokā khuddamālakā;",
  "tha-ap407:4.3_0": "Punnāgā giripunnāgā,",
  "tha-ap407:4.4_0": "sampavanti mamassamaṁ.",
  "tha-ap407:5.1_0": "Kuṭajā pupphitā tattha,",
  "tha-ap407:5.2_0": "tiṇasūlavanāni ca;",
  "tha-ap407:5.3_0": "Sālā ca saḷalā tattha,",
  "tha-ap407:5.4_0": "campakā pupphitā bahū.",
  "tha-ap407:6.1_0": "Ajjunā atimuttā ca,",
  "tha-ap407:6.2_0": "mahānāmā ca pupphitā;",
  "tha-ap407:6.3_0": "Asanā madhugandhī ca,",
  "tha-ap407:6.4_0": "pupphitā te mamassame.",
  "tha-ap407:7.1_0": "Uddālakā pāṭalikā,",
  "tha-ap407:7.2_0": "yūthikā ca piyaṅgukā;",
  "tha-ap407:7.3_0": "Bimbijālakasañchannā,",
  "tha-ap407:7.4_0": "samantā aḍḍhayojanaṁ.",
  "tha-ap407:8.1_0": "Mātaggārā sattaliyo,",
  "tha-ap407:8.2_0": "pāṭalī sinduvārakā;",
  "tha-ap407:8.3_0": "Aṅkolakā bahū tattha,",
  "tha-ap407:8.4_0": "tālakuṭṭhi ca pupphitā;",
  "tha-ap407:8.5_0": "Seleyyakā bahū tattha,",
  "tha-ap407:8.6_0": "pupphitā mama assame.",
  "tha-ap407:9.1_0": "Etesu pupphajātesu,",
  "tha-ap407:9.2_0": "sobhanti pādapā bahū;",
  "tha-ap407:9.3_0": "Samantā tena gandhena,",
  "tha-ap407:9.4_0": "vāyate mama assamo.",
  "tha-ap407:10.1_0": "Harītakā āmalakā,",
  "tha-ap407:10.2_0": "ambajambuvibhītakā;",
  "tha-ap407:10.3_0": "Kolā bhallātakā billā,",
  "tha-ap407:10.4_0": "phārusakaphalāni ca.",
  "tha-ap407:11.1_0": "Tindukā ca piyālā ca,",
  "tha-ap407:11.2_0": "madhukā kāsumārayo;",
  "tha-ap407:11.3_0": "Labujā panasā tattha,",
  "tha-ap407:11.4_0": "kadalī badarīphalā.",
  "tha-ap407:12.1_0": "Ambāṭakā bahū tattha,",
  "tha-ap407:12.2_0": "vallikāraphalāni ca;",
  "tha-ap407:12.3_0": "Bījapūrasapāriyo,",
  "tha-ap407:12.4_0": "phalitā mama assame.",
  "tha-ap407:13.1_0": "Āḷakā isimuggā ca,",
  "tha-ap407:13.2_0": "tato modaphalā bahū;",
  "tha-ap407:13.3_0": "Avaṭā pakkabharitā,",
  "tha-ap407:13.4_0": "pilakkhudumbarāni ca.",
  "tha-ap407:14.1_0": "Pipphalī marīcā tattha,",
  "tha-ap407:14.2_0": "nigrodhā ca kapitthanā;",
  "tha-ap407:14.3_0": "Udumbarakā bahavo,",
  "tha-ap407:14.4_0": "kaṇḍupaṇṇā ca hariyo.",
  "tha-ap407:15.1_0": "Ete caññe ca bahavo,",
  "tha-ap407:15.2_0": "phalitā assame mama;",
  "tha-ap407:15.3_0": "Puppharukkhāpi bahavo,",
  "tha-ap407:15.4_0": "pupphitā mama assame.",
  "tha-ap407:16.1_0": "Āluvā ca kaḷambā ca,",
  "tha-ap407:16.2_0": "biḷālī takkalāni ca;",
  "tha-ap407:16.3_0": "Ālakā tālakā ceva,",
  "tha-ap407:16.4_0": "vijjanti assame mama.",
  "tha-ap407:17.1_0": "Assamassāvidūre me,",
  "tha-ap407:17.2_0": "mahājātassaro ahu;",
  "tha-ap407:17.3_0": "Acchodako sītajalo,",
  "tha-ap407:17.4_0": "supatittho manoramo.",
  "tha-ap407:18.1_0": "Padumuppalā bahū tattha,",
  "tha-ap407:18.2_0": "puṇḍarīkasamāyutā;",
  "tha-ap407:18.3_0": "Mandālakehi sañchannā,",
  "tha-ap407:18.4_0": "nānāgandhasameritā.",
  "tha-ap407:19.1_0": "Gabbhaṁ gaṇhanti padumā,",
  "tha-ap407:19.2_0": "aññe pupphanti kesarī;",
  "tha-ap407:19.3_0": "Opupphapattā tiṭṭhanti,",
  "tha-ap407:19.4_0": "padumākaṇṇikā bahū.",
  "tha-ap407:20.1_0": "Madhu bhisamhā savati,",
  "tha-ap407:20.2_0": "khīraṁ sappi mulāḷibhi;",
  "tha-ap407:20.3_0": "Samantā tena gandhena,",
  "tha-ap407:20.4_0": "nānāgandhasameritā.",
  "tha-ap407:21.1_0": "Kumudā ambagandhi ca,",
  "tha-ap407:21.2_0": "nayitā dissare bahū;",
  "tha-ap407:21.3_0": "Jātassarassānukūlaṁ,",
  "tha-ap407:21.4_0": "ketakā pupphitā bahū.",
  "tha-ap407:22.1_0": "Suphullā bandhujīvā ca,",
  "tha-ap407:22.2_0": "setavārī sugandhikā;",
  "tha-ap407:22.3_0": "Kumbhilā susumārā ca,",
  "tha-ap407:22.4_0": "gahakā tattha jāyare.",
  "tha-ap407:23.1_0": "Uggāhakā ajagarā,",
  "tha-ap407:23.2_0": "tattha jātassare bahū;",
  "tha-ap407:23.3_0": "Pāṭhīnā pāvusā macchā,",
  "tha-ap407:23.4_0": "balajā muñjarohitā.",
  "tha-ap407:24.1_0": "Macchakacchapasañchannā,",
  "tha-ap407:24.2_0": "atho papaṭakāhi ca;",
  "tha-ap407:24.3_0": "Pārevatā ravihaṁsā,",
  "tha-ap407:24.4_0": "kukutthā ca nadīcarā.",
  "tha-ap407:25.1_0": "Dindibhā cakkavākā ca,",
  "tha-ap407:25.2_0": "pampakā jīvajīvakā;",
  "tha-ap407:25.3_0": "Kalandakā ukkusā ca,",
  "tha-ap407:25.4_0": "senakā uddharā bahū.",
  "tha-ap407:26.1_0": "Koṭṭhakā sukapotā ca,",
  "tha-ap407:26.2_0": "tuliyā camarā bahū;",
  "tha-ap407:26.3_0": "Kāreniyo ca tilakā,",
  "tha-ap407:26.4_0": "upajīvanti taṁ saraṁ.",
  "tha-ap407:27.1_0": "Sīhā byagghā ca dīpī ca,",
  "tha-ap407:27.2_0": "acchakokataracchakā;",
  "tha-ap407:27.3_0": "Vānarā kinnarā ceva,",
  "tha-ap407:27.4_0": "dissanti mama assame.",
  "tha-ap407:28.1_0": "Tāni gandhāni ghāyanto,",
  "tha-ap407:28.2_0": "bhakkhayanto phalānahaṁ;",
  "tha-ap407:28.3_0": "Gandhodakaṁ pivanto ca,",
  "tha-ap407:28.4_0": "vasāmi mama assame.",
  "tha-ap407:29.1_0": "Eṇīmigā varāhā ca,",
  "tha-ap407:29.2_0": "pasadā khuddarūpakā;",
  "tha-ap407:29.3_0": "Aggikā jotikā ceva,",
  "tha-ap407:29.4_0": "vasanti mama assame.",
  "tha-ap407:30.1_0": "Haṁsā koñcā mayūrā ca,",
  "tha-ap407:30.2_0": "sālikāpi ca kokilā;",
  "tha-ap407:30.3_0": "Majjārikā bahū tattha,",
  "tha-ap407:30.4_0": "kosikā poṭṭhasīsakā.",
  "tha-ap407:31.1_0": "Pisācā dānavā ceva,",
  "tha-ap407:31.2_0": "kumbhaṇḍā rakkhasā bahū;",
  "tha-ap407:31.3_0": "Garuḷā pannagā ceva,",
  "tha-ap407:31.4_0": "vasanti mama assame.",
  "tha-ap407:32.1_0": "Mahānubhāvā isayo,",
  "tha-ap407:32.2_0": "santacittā samāhitā;",
  "tha-ap407:32.3_0": "Kamaṇḍaludharā sabbe,",
  "tha-ap407:32.4_0": "ajinuttaravāsanā;",
  "tha-ap407:32.5_0": "Jaṭābhārabharitāva,",
  "tha-ap407:32.6_0": "vasanti mama assame.",
  "tha-ap407:33.1_0": "Yugamattañca pekkhantā,",
  "tha-ap407:33.2_0": "nipakā santavuttino;",
  "tha-ap407:33.3_0": "Lābhālābhena santuṭṭhā,",
  "tha-ap407:33.4_0": "vasanti mama assame.",
  "tha-ap407:34.1_0": "Vākacīraṁ dhunantā te,",
  "tha-ap407:34.2_0": "phoṭentājinacammakaṁ;",
  "tha-ap407:34.3_0": "Sabalehi upatthaddhā,",
  "tha-ap407:34.4_0": "gacchanti ambare tadā.",
  "tha-ap407:35.1_0": "Na te dakaṁ āharanti,",
  "tha-ap407:35.2_0": "kaṭṭhaṁ vā aggidārukaṁ;",
  "tha-ap407:35.3_0": "Sayañca upasampannā,",
  "tha-ap407:35.4_0": "pāṭihīrassidaṁ phalaṁ.",
  "tha-ap407:36.1_0": "Lohadoṇiṁ gahetvāna,",
  "tha-ap407:36.2_0": "vanamajjhe vasanti te;",
  "tha-ap407:36.3_0": "Kuñjarāva mahānāgā,",
  "tha-ap407:36.4_0": "asambhītāva kesarī.",
  "tha-ap407:37.1_0": "Aññe gacchanti goyānaṁ,",
  "tha-ap407:37.2_0": "aññe pubbavidehakaṁ;",
  "tha-ap407:37.3_0": "Aññe ca uttarakuruṁ,",
  "tha-ap407:37.4_0": "sakaṁ balamavassitā.",
  "tha-ap407:38.1_0": "Tato piṇḍaṁ āharitvā,",
  "tha-ap407:38.2_0": "paribhuñjanti ekato;",
  "tha-ap407:38.3_0": "Sabbesaṁ pakkamantānaṁ,",
  "tha-ap407:38.4_0": "uggatejāna tādinaṁ.",
  "tha-ap407:39.1_0": "Ajinacammasaddena,",
  "tha-ap407:39.2_0": "vanaṁ saddāyate tadā;",
  "tha-ap407:39.3_0": "Edisā te mahāvīra,",
  "tha-ap407:39.4_0": "sissā uggatapā mama.",
  "tha-ap407:40.1_0": "Parivuto ahaṁ tehi,",
  "tha-ap407:40.2_0": "vasāmi mama assame;",
  "tha-ap407:40.3_0": "Tositā sakakammena,",
  "tha-ap407:40.4_0": "vinītāpi samāgatā.",
  "tha-ap407:41.1_0": "Ārādhayiṁsu maṁ ete,",
  "tha-ap407:41.2_0": "sakakammābhilāsino;",
  "tha-ap407:41.3_0": "Sīlavanto ca nipakā,",
  "tha-ap407:41.4_0": "appamaññāsu kovidā.",
  "tha-ap407:42.1_0": "Padumuttaro lokavidū,",
  "tha-ap407:42.2_0": "āhutīnaṁ paṭiggaho;",
  "tha-ap407:42.3_0": "Samayaṁ saṁviditvāna,",
  "tha-ap407:42.4_0": "upagacchi vināyako.",
  "tha-ap407:43.1_0": "Upagantvāna sambuddho,",
  "tha-ap407:43.2_0": "ātāpī nipako muni;",
  "tha-ap407:43.3_0": "Pattaṁ paggayha sambuddho,",
  "tha-ap407:43.4_0": "bhikkhāya mamupāgami.",
  "tha-ap407:44.1_0": "Upāgataṁ mahāvīraṁ,",
  "tha-ap407:44.2_0": "jalajuttamanāyakaṁ;",
  "tha-ap407:44.3_0": "Tiṇasantharaṁ paññāpetvā,",
  "tha-ap407:44.4_0": "sālapupphehi okiriṁ.",
  "tha-ap407:45.1_0": "Nisādetvāna sambuddhaṁ,",
  "tha-ap407:45.2_0": "haṭṭho saṁviggamānaso;",
  "tha-ap407:45.3_0": "Khippaṁ pabbatamāruyha,",
  "tha-ap407:45.4_0": "agaḷuṁ aggahiṁ ahaṁ.",
  "tha-ap407:46.1_0": "Kumbhamattaṁ gahetvāna,",
  "tha-ap407:46.2_0": "panasaṁ devagandhikaṁ;",
  "tha-ap407:46.3_0": "Khandhe āropayitvāna,",
  "tha-ap407:46.4_0": "upagacchiṁ vināyakaṁ.",
  "tha-ap407:47.1_0": "Phalaṁ buddhassa datvāna,",
  "tha-ap407:47.2_0": "agaḷuṁ anulimpahaṁ;",
  "tha-ap407:47.3_0": "Pasannacitto sumano,",
  "tha-ap407:47.4_0": "buddhaseṭṭhaṁ avandihaṁ.",
  "tha-ap407:48.1_0": "Padumuttaro lokavidū,",
  "tha-ap407:48.2_0": "āhutīnaṁ paṭiggaho;",
  "tha-ap407:48.3_0": "Isimajjhe nisīditvā,",
  "tha-ap407:48.4_0": "imā gāthā abhāsatha.",
  "tha-ap407:49.1_0": "‘Yo me phalañca agaḷuṁ,",
  "tha-ap407:49.2_0": "āsanañca adāsi me;",
  "tha-ap407:49.3_0": "Tamahaṁ kittayissāmi,",
  "tha-ap407:49.4_0": "suṇātha mama bhāsato.",
  "tha-ap407:50.1_0": "Gāme vā yadi vāraññe,",
  "tha-ap407:50.2_0": "pabbhāresu guhāsu vā;",
  "tha-ap407:50.3_0": "Imassa cittamaññāya,",
  "tha-ap407:50.4_0": "nibbattissati bhojanaṁ.",
  "tha-ap407:51.1_0": "Devaloke manusse vā,",
  "tha-ap407:51.2_0": "upapanno ayaṁ naro;",
  "tha-ap407:51.3_0": "Bhojanehi ca vatthehi,",
  "tha-ap407:51.4_0": "parisaṁ tappayissati.",
  "tha-ap407:52.1_0": "Upapajjati yaṁ yoniṁ,",
  "tha-ap407:52.2_0": "devattaṁ atha mānusaṁ;",
  "tha-ap407:52.3_0": "Akkhobhabhogo hutvāna,",
  "tha-ap407:52.4_0": "saṁsarissatiyaṁ naro.",
  "tha-ap407:53.1_0": "Tiṁsakappasahassāni,",
  "tha-ap407:53.2_0": "devaloke ramissati;",
  "tha-ap407:53.3_0": "Sahassakkhattuṁ rājā ca,",
  "tha-ap407:53.4_0": "cakkavattī bhavissati.",
  "tha-ap407:54.1_0": "Ekasattatikkhattuñca,",
  "tha-ap407:54.2_0": "devarajjaṁ karissati;",
  "tha-ap407:54.3_0": "Padesarajjaṁ vipulaṁ,",
  "tha-ap407:54.4_0": "gaṇanāto asaṅkhiyaṁ.",
  "tha-ap407:55.1_0": "Kappasatasahassamhi,",
  "tha-ap407:55.2_0": "okkākakulasambhavo;",
  "tha-ap407:55.3_0": "Gotamo nāma gottena,",
  "tha-ap407:55.4_0": "satthā loke bhavissati.",
  "tha-ap407:56.1_0": "Tassa dhammesu dāyādo,",
  "tha-ap407:56.2_0": "oraso dhammanimmito;",
  "tha-ap407:56.3_0": "(…)",
  "tha-ap407:56.4_0": "Sabbāsave pariññāya,",
  "tha-ap407:56.5_0": "viharissatināsavo’.",
  "tha-ap407:57.1_0": "Suladdhalābho laddho me,",
  "tha-ap407:57.2_0": "yohaṁ addakkhiṁ nāyakaṁ;",
  "tha-ap407:57.3_0": "Tisso vijjā anuppattā,",
  "tha-ap407:57.4_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap407:58.1_0": "Gāme vā yadi vāraññe,",
  "tha-ap407:58.2_0": "pabbhāresu guhāsu vā;",
  "tha-ap407:58.3_0": "Mama saṅkappamaññāya,",
  "tha-ap407:58.4_0": "bhojanaṁ hoti me sadā.",
  "tha-ap407:59.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap407:59.2_0": "…pe…",
  "tha-ap407:59.3_0": "viharāmi anāsavo.",
  "tha-ap407:60.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap407:60.2_0": "…pe…",
  "tha-ap407:60.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap407:61.1_0": "Paṭisambhidā catasso,",
  "tha-ap407:61.2_0": "…pe…",
  "tha-ap407:61.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap407:62.1_0": "Itthaṁ sudaṁ āyasmā upasivo thero imā gāthāyo abhāsitthāti.",
  "tha-ap407:63.1_0": "Upasivattherassāpadānaṁ pañcamaṁ."
}