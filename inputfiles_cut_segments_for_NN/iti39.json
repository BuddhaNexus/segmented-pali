{
  "iti39:0.1_0": "Itivuttaka 39",
  "iti39:0.2_0": "Dukanipāta",
  "iti39:0.3_0": "Dutiyavagga",
  "iti39:0.4_0": "Desanāsutta",
  "iti39:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti39:2.1_0": "“Tathāgatassa, bhikkhave,",
  "iti39:2.1_1": "arahato sammāsambuddhassa dve dhammadesanā pariyāyena bhavanti.",
  "iti39:2.2_0": "Katamā dve?",
  "iti39:2.3_0": "‘Pāpaṁ pāpakato passathā’ti—",
  "iti39:2.4_0": "ayaṁ paṭhamā dhammadesanā;",
  "iti39:2.5_0": "‘pāpaṁ pāpakato disvā tattha nibbindatha virajjatha vimuccathā’ti—",
  "iti39:2.6_0": "ayaṁ dutiyā dhammadesanā.",
  "iti39:2.7_0": "Tathāgatassa, bhikkhave,",
  "iti39:2.7_1": "arahato sammāsambuddhassa imā dve dhammadesanā pariyāyena bhavantī”ti.",
  "iti39:2.8_0": "Etamatthaṁ bhagavā avoca.",
  "iti39:2.9_0": "Tatthetaṁ iti vuccati:",
  "iti39:3.1_0": "“Tathāgatassa buddhassa,",
  "iti39:3.2_0": "sabbabhūtānukampino;",
  "iti39:3.3_0": "Pariyāyavacanaṁ passa,",
  "iti39:3.4_0": "dve ca dhammā pakāsitā.",
  "iti39:4.1_0": "Pāpakaṁ passatha cetaṁ,",
  "iti39:4.2_0": "tattha cāpi virajjatha;",
  "iti39:4.3_0": "Tato virattacittāse,",
  "iti39:4.4_0": "dukkhassantaṁ karissathā”ti.",
  "iti39:5.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti39:5.2_0": "Dutiyaṁ."
}