{
  "tha-ap313:0.1_0": "Therāpadāna",
  "tha-ap313:0.2_0": "Ārakkhadāyakavagga",
  "tha-ap313:0.3_0": "1. Ārakkhadāyakattheraapadāna",
  "tha-ap313:1.1_0": "“Dhammadassissa munino,",
  "tha-ap313:1.2_0": "vati kārāpitā mayā;",
  "tha-ap313:1.3_0": "Ārakkho ca mayā dinno,",
  "tha-ap313:1.4_0": "dvipadindassa tādino.",
  "tha-ap313:2.1_0": "Aṭṭhārase kappasate,",
  "tha-ap313:2.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap313:2.3_0": "Tena kammavisesena,",
  "tha-ap313:2.4_0": "patto me āsavakkhayo.",
  "tha-ap313:3.1_0": "Paṭisambhidā catasso,",
  "tha-ap313:3.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap313:3.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap313:3.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap313:4.1_0": "Itthaṁ sudaṁ āyasmā ārakkhadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap313:5.1_0": "Ārakkhadāyakattherassāpadānaṁ paṭhamaṁ."
}