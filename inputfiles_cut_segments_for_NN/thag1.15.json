{
  "thag1.15:0.1_0": "Theragāthā",
  "thag1.15:0.2_0": "Ekakanipāta",
  "thag1.15:0.3_0": "Dutiyavagga",
  "thag1.15:0.4_0": "1.15. Kuṇḍadhānattheragāthā",
  "thag1.15:1.1_0": "“Pañca chinde pañca jahe,",
  "thag1.15:1.2_0": "pañca cuttari bhāvaye;",
  "thag1.15:1.3_0": "Pañcasaṅgātigo bhikkhu,",
  "thag1.15:1.4_0": "oghatiṇṇoti vuccatī”ti.",
  "thag1.15:1.5_0": "… Kuṇḍadhāno thero …."
}