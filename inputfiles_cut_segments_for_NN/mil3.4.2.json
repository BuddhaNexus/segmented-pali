{
  "mil3.4.2:0.1_0": "Milindapañha",
  "mil3.4.2:0.2_0": "Nibbānavagga",
  "mil3.4.2:0.3_0": "2. Nāgasenapañha",
  "mil3.4.2:1.1_0": "Thero āha—",
  "mil3.4.2:1.2_0": "“loṇaṁ, mahārāja, cakkhuviññeyyan”ti.",
  "mil3.4.2:1.3_0": "“Āma, bhante, cakkhuviññeyyan”ti.",
  "mil3.4.2:1.4_0": "“Suṭṭhu kho, mahārāja, jānāhī”ti.",
  "mil3.4.2:1.5_0": "“Kiṁ pana, bhante, jivhāviññeyyan”ti?",
  "mil3.4.2:1.6_0": "“Āma, mahārāja, jivhāviññeyyan”ti.",
  "mil3.4.2:1.7_0": "“Kiṁ pana, bhante,",
  "mil3.4.2:1.7_1": "sabbaṁ loṇaṁ jivhāya vijānātī”ti?",
  "mil3.4.2:1.8_0": "“Āma, mahārāja, sabbaṁ loṇaṁ jivhāya vijānāti”.",
  "mil3.4.2:2.1_0": "“Yadi, bhante,",
  "mil3.4.2:2.1_1": "sabbaṁ loṇaṁ jivhāya vijānāti,",
  "mil3.4.2:2.1_2": "kissa pana taṁ sakaṭehi balībaddā āharanti, nanu loṇameva āharitabban”ti?",
  "mil3.4.2:2.2_0": "“Na sakkā, mahārāja,",
  "mil3.4.2:2.2_1": "loṇameva āharituṁ ekatobhāvagatā ete dhammā gocaranānattagatā loṇaṁ garubhāvo cāti.",
  "mil3.4.2:2.3_0": "Sakkā pana, mahārāja, loṇaṁ tulāya tulayitun”ti?",
  "mil3.4.2:2.4_0": "“Āma, bhante, sakkā”ti.",
  "mil3.4.2:2.5_0": "“Na sakkā, mahārāja, loṇaṁ tulāya tulayituṁ, garubhāvo tulāya tuliyatī”ti.",
  "mil3.4.2:3.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.4.2:4.1_0": "Nāgasenapañho dutiyo."
}