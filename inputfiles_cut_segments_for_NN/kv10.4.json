{
  "kv10.4:0.1_0": "Kathāvatthu",
  "kv10.4:0.2_0": "Mahāpaṇṇāsaka",
  "kv10.4:0.3_0": "Dasamavagga",
  "kv10.4:0.4_0": "Pañcaviññāṇākusalāpiakusalāpītikathā",
  "kv10.4:1.1_0": "Pañcaviññāṇā kusalāpi akusalāpīti?",
  "kv10.4:1.2_0": "Āmantā.",
  "kv10.4:1.3_0": "Nanu pañcaviññāṇā uppannavatthukā uppannārammaṇāti?",
  "kv10.4:1.4_0": "Āmantā.",
  "kv10.4:1.5_0": "Hañci pañcaviññāṇā uppannavatthukā uppannārammaṇā, no ca vata re vattabbe—",
  "kv10.4:1.6_0": "“pañcaviññāṇā kusalāpi akusalāpī”ti.",
  "kv10.4:1.7_0": "Nanu pañcaviññāṇā purejātavatthukā purejātārammaṇā ajjhattikavatthukā bāhirārammaṇā asambhinnavatthukā asambhinnārammaṇā nānāvatthukā nānārammaṇā na aññamaññassa gocaravisayaṁ paccanubhonti, na asamannāhārā uppajjanti, na amanasikārā uppajjanti, na abbokiṇṇā uppajjanti,",
  "kv10.4:1.7_1": "na apubbaṁ acarimaṁ uppajjanti,",
  "kv10.4:1.7_2": "na aññamaññassa samanantarā uppajjanti, nanu pañcaviññāṇā anābhogāti?",
  "kv10.4:1.8_0": "Āmantā.",
  "kv10.4:1.9_0": "Hañci pañcaviññāṇā anābhogā, no ca vata re vattabbe—",
  "kv10.4:1.10_0": "“pañcaviññāṇā kusalāpi akusalāpī”ti.",
  "kv10.4:2.1_0": "Cakkhuviññāṇaṁ kusalanti?",
  "kv10.4:2.2_0": "Āmantā.",
  "kv10.4:2.3_0": "Cakkhuviññāṇaṁ suññataṁ ārabbha uppajjatīti?",
  "kv10.4:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv10.4:2.5_0": "cakkhuviññāṇaṁ suññataṁ ārabbha uppajjatīti?",
  "kv10.4:2.6_0": "Āmantā.",
  "kv10.4:2.7_0": "Cakkhuñca paṭicca suññatañca uppajjati cakkhuviññāṇanti?",
  "kv10.4:2.8_0": "Na hevaṁ vattabbe.",
  "kv10.4:2.9_0": "Cakkhuñca paṭicca suññatañca uppajjati cakkhuviññāṇanti?",
  "kv10.4:2.10_0": "Āmantā.",
  "kv10.4:2.11_0": "“Cakkhuñca paṭicca suññatañca uppajjati cakkhuviññāṇan”ti—",
  "kv10.4:2.12_0": "attheva suttantoti?",
  "kv10.4:2.13_0": "Natthi.",
  "kv10.4:2.14_0": "“Cakkhuñca paṭicca rūpe ca uppajjati cakkhuviññāṇan”ti—",
  "kv10.4:2.15_0": "attheva suttantoti?",
  "kv10.4:2.16_0": "Āmantā.",
  "kv10.4:2.17_0": "Hañci “cakkhuñca paṭicca rūpe ca uppajjati cakkhuviññāṇan”ti—",
  "kv10.4:2.18_0": "attheva suttanto, no ca vata re vattabbe—",
  "kv10.4:2.19_0": "“cakkhuñca paṭicca suññatañca uppajjati cakkhuviññāṇan”ti.",
  "kv10.4:3.1_0": "Cakkhuviññāṇaṁ kusalampi akusalampīti?",
  "kv10.4:3.2_0": "Āmantā.",
  "kv10.4:3.3_0": "Cakkhuviññāṇaṁ atītānāgataṁ ārabbha uppajjatīti?",
  "kv10.4:3.4_0": "Na hevaṁ vattabbe …pe….",
  "kv10.4:3.5_0": "Cakkhuviññāṇaṁ kusalampi akusalampīti?",
  "kv10.4:3.6_0": "Āmantā.",
  "kv10.4:3.7_0": "Cakkhuviññāṇaṁ phassaṁ ārabbha …pe…",
  "kv10.4:3.8_0": "cittaṁ ārabbha … cakkhuṁ ārabbha …pe…",
  "kv10.4:3.9_0": "kāyaṁ ārabbha … saddaṁ ārabbha …pe…",
  "kv10.4:3.10_0": "phoṭṭhabbaṁ ārabbha uppajjatīti?",
  "kv10.4:3.11_0": "Na hevaṁ vattabbe …pe….",
  "kv10.4:4.1_0": "Manoviññāṇaṁ kusalampi akusalampi,",
  "kv10.4:4.1_1": "manoviññāṇaṁ suññataṁ ārabbha uppajjatīti?",
  "kv10.4:4.2_0": "Āmantā.",
  "kv10.4:4.3_0": "Cakkhuviññāṇaṁ kusalampi akusalampi,",
  "kv10.4:4.3_1": "cakkhuviññāṇaṁ suññataṁ ārabbha uppajjatīti?",
  "kv10.4:4.4_0": "Na hevaṁ vattabbe …pe…",
  "kv10.4:4.5_0": "manoviññāṇaṁ kusalampi akusalampi,",
  "kv10.4:4.5_1": "manoviññāṇaṁ atītānāgataṁ ārabbha uppajjatīti?",
  "kv10.4:4.6_0": "Āmantā.",
  "kv10.4:4.7_0": "Cakkhuviññāṇaṁ kusalampi akusalampi,",
  "kv10.4:4.7_1": "cakkhuviññāṇaṁ atītānāgataṁ ārabbha uppajjatīti?",
  "kv10.4:4.8_0": "Na hevaṁ vattabbe …pe…",
  "kv10.4:4.9_0": "manoviññāṇaṁ kusalampi akusalampi,",
  "kv10.4:4.9_1": "manoviññāṇaṁ phassaṁ ārabbha …pe…",
  "kv10.4:4.10_0": "cittaṁ ārabbha …pe…",
  "kv10.4:4.11_0": "kāyaṁ ārabbha … saddaṁ ārabbha …pe…",
  "kv10.4:4.12_0": "phoṭṭhabbaṁ ārabbha uppajjatīti?",
  "kv10.4:4.13_0": "Āmantā.",
  "kv10.4:4.14_0": "Cakkhuviññāṇaṁ kusalampi akusalampi,",
  "kv10.4:4.14_1": "cakkhuviññāṇaṁ phassaṁ ārabbha …pe…",
  "kv10.4:4.15_0": "phoṭṭhabbaṁ ārabbha uppajjatīti?",
  "kv10.4:4.16_0": "Na hevaṁ vattabbe …pe….",
  "kv10.4:5.1_0": "Na vattabbaṁ—",
  "kv10.4:5.2_0": "“pañcaviññāṇā kusalāpi akusalāpī”ti?",
  "kv10.4:5.3_0": "Āmantā.",
  "kv10.4:5.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv10.4:5.5_0": "“idha, bhikkhave,",
  "kv10.4:5.5_1": "bhikkhu cakkhunā rūpaṁ disvā nimittaggāhī hoti …pe…",
  "kv10.4:5.6_0": "na nimittaggāhī hoti …pe…",
  "kv10.4:5.7_0": "sotena saddaṁ sutvā …pe…",
  "kv10.4:5.8_0": "kāyena phoṭṭhabbaṁ phusitvā nimittaggāhī hoti …pe…",
  "kv10.4:5.9_0": "na nimittaggāhī hotī”ti.",
  "kv10.4:5.10_0": "Attheva suttantoti?",
  "kv10.4:5.11_0": "Āmantā.",
  "kv10.4:5.12_0": "Tena hi pañcaviññāṇā kusalāpi akusalāpīti.",
  "kv10.4:5.13_0": "Pañcaviññāṇā kusalāpi akusalāpītikathā niṭṭhitā."
}