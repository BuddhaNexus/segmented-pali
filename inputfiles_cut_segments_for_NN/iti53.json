{
  "iti53:0.1_0": "Itivuttaka 53",
  "iti53:0.2_0": "Tikanipāta",
  "iti53:0.3_0": "Paṭhamavagga",
  "iti53:0.4_0": "Dutiyavedanāsutta",
  "iti53:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti53:2.1_0": "“Tisso imā, bhikkhave, vedanā.",
  "iti53:2.2_0": "Katamā tisso?",
  "iti53:2.3_0": "Sukhā vedanā, dukkhā vedanā, adukkhamasukhā vedanā.",
  "iti53:2.4_0": "Sukhā, bhikkhave, vedanā dukkhato daṭṭhabbā;",
  "iti53:2.5_0": "dukkhā vedanā sallato daṭṭhabbā;",
  "iti53:2.6_0": "adukkhamasukhā vedanā aniccato daṭṭhabbā.",
  "iti53:2.7_0": "Yato kho, bhikkhave,",
  "iti53:2.7_1": "bhikkhuno sukhā vedanā dukkhato diṭṭhā hoti,",
  "iti53:2.7_2": "dukkhā vedanā sallato diṭṭhā hoti,",
  "iti53:2.7_3": "adukkhamasukhā vedanā aniccato diṭṭhā hoti;",
  "iti53:2.8_0": "ayaṁ vuccati, bhikkhave,",
  "iti53:2.8_1": "‘bhikkhu ariyo sammaddaso acchecchi, taṇhaṁ, vivattayi saṁyojanaṁ,",
  "iti53:2.8_2": "sammā mānābhisamayā antamakāsi dukkhassā’”ti.",
  "iti53:2.9_0": "Etamatthaṁ bhagavā avoca.",
  "iti53:2.10_0": "Tatthetaṁ iti vuccati:",
  "iti53:3.1_0": "“Yo sukhaṁ dukkhato adda,",
  "iti53:3.2_0": "Dukkhamaddakkhi sallato;",
  "iti53:3.3_0": "Adukkhamasukhaṁ santaṁ,",
  "iti53:3.4_0": "Adakkhi naṁ aniccato.",
  "iti53:4.1_0": "Sa ve sammaddaso bhikkhu,",
  "iti53:4.2_0": "yato tattha vimuccati;",
  "iti53:4.3_0": "Abhiññāvosito santo,",
  "iti53:4.4_0": "sa ve yogātigo munī”ti.",
  "iti53:5.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti53:5.2_0": "Catutthaṁ."
}