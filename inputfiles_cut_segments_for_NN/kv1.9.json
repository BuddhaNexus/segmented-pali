  "kv1.9:2.26_0": "ahirikaṁ …",
  "kv1.9:2.27_0": "anottappaṁ satipaṭṭhānanti?",
  "kv1.9:2.28_0": "Na hevaṁ vattabbe …pe…",
  "kv1.9:2.29_0": "anottappaṁ satipaṭṭhānanti?",
  "kv1.9:2.30_0": "Āmantā.",
  "kv1.9:2.31_0": "Anottappaṁ sati satindriyaṁ satibalaṁ sammāsati …pe… kāyagatāsati upasamānussatīti?",
  "kv1.9:2.32_0": "Na hevaṁ vattabbe …pe….",
  "kv1.9:3.1_0": "Sati satipaṭṭhānā, sā ca satīti?",
  "kv1.9:3.2_0": "Āmantā.",
  "kv1.9:3.3_0": "Cakkhāyatanaṁ satipaṭṭhānaṁ, tañca satīti?",
  "kv1.9:3.4_0": "Na hevaṁ vattabbe …pe…",
  "kv1.9:3.5_0": "sati satipaṭṭhānā, sā ca satīti?",
  "kv1.9:3.6_0": "Āmantā.",
  "kv1.9:3.7_0": "Sotāyatanaṁ …pe…",
  "kv1.9:3.8_0": "kāyāyatanaṁ … rūpāyatanaṁ …pe…",
  "kv1.9:3.9_0": "phoṭṭhabbāyatanaṁ … rāgo … doso … moho …pe…",
  "kv1.9:3.10_0": "anottappaṁ satipaṭṭhānaṁ, tañca satīti?",
  "kv1.9:3.11_0": "Na hevaṁ vattabbe …pe….",
  "kv1.9:4.1_0": "Cakkhāyatanaṁ satipaṭṭhānaṁ, tañca na satīti?",
  "kv1.9:4.2_0": "Āmantā.",
  "kv1.9:4.3_0": "Sati satipaṭṭhānā, sā ca na satīti?",
  "kv1.9:4.4_0": "Na hevaṁ vattabbe …pe…",
  "kv1.9:4.5_0": "sotāyatanaṁ …pe…",
  "kv1.9:4.6_0": "kāyāyatanaṁ …",
  "kv1.9:4.7_0": "rūpāyatanaṁ …pe…",
  "kv1.9:4.8_0": "phoṭṭhabbāyatanaṁ …",
  "kv1.9:4.9_0": "rāgo …",
  "kv1.9:4.10_0": "doso …",
  "kv1.9:4.11_0": "moho …pe…",
  "kv1.9:4.12_0": "anottappaṁ satipaṭṭhānaṁ, tañca na satīti?",
  "kv1.9:4.13_0": "Āmantā.",
  "kv1.9:4.14_0": "Sati satipaṭṭhānā, sā ca na satīti?",
  "kv1.9:4.15_0": "Na hevaṁ vattabbe …pe….",
  "kv1.9:5.1_0": "Na vattabbaṁ—",
  "kv1.9:5.2_0": "“sabbe dhammā satipaṭṭhānā”ti?",
  "kv1.9:5.3_0": "Āmantā.",
  "kv1.9:5.4_0": "Nanu sabbe dhamme ārabbha sati santiṭṭhatīti?",
  "kv1.9:5.5_0": "Āmantā.",
  "kv1.9:5.6_0": "Hañci sabbe dhamme ārabbha sati santiṭṭhatīti, tena vata re vattabbe—",
  "kv1.9:5.7_0": "“sabbe dhammā satipaṭṭhānā”ti.",
  "kv1.9:6.1_0": "Sabbaṁ dhammaṁ ārabbha sati santiṭṭhatīti sabbe dhammā satipaṭṭhānāti?",
  "kv1.9:6.2_0": "Āmantā.",
  "kv1.9:6.3_0": "Sabbaṁ dhammaṁ ārabbha phasso santiṭṭhatīti sabbe dhammā phassapaṭṭhānāti?",
  "kv1.9:6.4_0": "Na hevaṁ vattabbe …pe….",
  "kv1.9:7.1_0": "Sabbaṁ dhammaṁ ārabbha sati santiṭṭhatīti sabbe dhammā satipaṭṭhānāti?",
  "kv1.9:7.2_0": "Āmantā.",
  "kv1.9:7.3_0": "Sabbaṁ dhammaṁ ārabbha vedanā santiṭṭhati …",
  "kv1.9:7.4_0": "saññā santiṭṭhati …",
  "kv1.9:7.5_0": "cetanā santiṭṭhati …",
  "kv1.9:7.6_0": "cittaṁ santiṭṭhatīti sabbe dhammā cittapaṭṭhānāti?",
  "kv1.9:7.7_0": "Na hevaṁ vattabbe …pe….",
  "kv1.9:8.1_0": "Sabbe dhammā satipaṭṭhānāti?",
  "kv1.9:8.2_0": "Āmantā.",
  "kv1.9:8.3_0": "Sabbe sattā upaṭṭhitasatino satiyā samannāgatā satiyā samohitā;",
  "kv1.9:8.4_0": "sabbesaṁ sattānaṁ sati paccupaṭṭhitāti?",
  "kv1.9:8.5_0": "Na hevaṁ vattabbe …pe….",
  "kv1.9:9.1_0": "Sabbe dhammā satipaṭṭhānāti?",
  "kv1.9:9.2_0": "Āmantā.",
  "kv1.9:9.3_0": "Nanu vuttaṁ bhagavatā—",
  "kv1.9:9.4_0": "“amataṁ te, bhikkhave,",
  "kv1.9:9.4_1": "na paribhuñjanti ye kāyagatāsatiṁ na paribhuñjanti.",
  "kv1.9:9.5_0": "Amataṁ te, bhikkhave,",
  "kv1.9:9.5_1": "paribhuñjanti ye kāyagatāsatiṁ paribhuñjantī”ti.",
  "kv1.9:9.6_0": "Attheva suttantoti?",
  "kv1.9:9.7_0": "Āmantā.",
  "kv1.9:9.8_0": "Sabbe sattā kāyagatāsatiṁ paribhuñjanti paṭilabhanti āsevanti bhāventi bahulīkarontīti?",
  "kv1.9:9.9_0": "Na hevaṁ vattabbe …pe….",
  "kv1.9:10.1_0": "Sabbe dhammā satipaṭṭhānāti?",
  "kv1.9:10.2_0": "Āmantā.",
  "kv1.9:10.3_0": "Nanu vuttaṁ bhagavatā—",
  "kv1.9:10.4_0": "“ekāyano ayaṁ, bhikkhave,",
  "kv1.9:10.4_1": "maggo sattānaṁ visuddhiyā sokaparidevānaṁ samatikkamāya dukkhadomanassānaṁ atthaṅgamāya ñāyassa adhigamāya nibbānassa sacchikiriyāya yadidaṁ cattāro satipaṭṭhānā”ti.",
  "kv1.9:10.5_0": "Attheva suttantoti?",
  "kv1.9:10.6_0": "Āmantā?",
  "kv1.9:10.7_0": "Sabbe dhammā ekāyanamaggoti?",
  "kv1.9:10.8_0": "Na hevaṁ vattabbe …pe….",
  "kv1.9:11.1_0": "Sabbe dhammā satipaṭṭhānāti?",
  "kv1.9:11.2_0": "Āmantā.",
  "kv1.9:11.3_0": "Nanu vuttaṁ bhagavatā—",
  "kv1.9:11.4_0": "“rañño, bhikkhave,",
  "kv1.9:11.4_1": "cakkavattissa pātubhāvā sattannaṁ ratanānaṁ pātubhāvo hoti.",
  "kv1.9:11.5_0": "Katamesaṁ sattannaṁ?",
  "kv1.9:11.6_0": "Cakkaratanassa pātubhāvo hoti,",
  "kv1.9:11.6_1": "hatthiratanassa pātubhāvo hoti, assaratanassa …",
  "kv1.9:11.7_0": "maṇiratanassa …",
  "kv1.9:11.8_0": "itthiratanassa …",
  "kv1.9:11.9_0": "gahapatiratanassa …",
  "kv1.9:11.10_0": "pariṇāyakaratanassa pātubhāvo hoti.",
  "kv1.9:11.11_0": "Rañño, bhikkhave,",
  "kv1.9:11.11_1": "cakkavattissa pātubhāvā imesaṁ sattannaṁ ratanānaṁ pātubhāvo hoti.",
  "kv1.9:12.1_0": "Tathāgatassa, bhikkhave,",
  "kv1.9:12.1_1": "pātubhāvā arahato sammāsambuddhassa sattannaṁ bojjhaṅgaratanānaṁ pātubhāvo hoti.",
  "kv1.9:12.2_0": "Katamesaṁ sattannaṁ?",
  "kv1.9:12.3_0": "Satisambojjhaṅgaratanassa pātubhāvo hoti,",
  "kv1.9:12.3_1": "dhammavicayasambojjhaṅgaratanassa pātubhāvo hoti,",
  "kv1.9:12.3_2": "vīriyasambojjhaṅgaratanassa pātubhāvo hoti,",
  "kv1.9:12.3_3": "pītisambojjhaṅgaratanassa pātubhāvo hoti,",
  "kv1.9:12.3_4": "passaddhisambojjhaṅgaratanassa pātubhāvo hoti,",
  "kv1.9:12.3_5": "samādhisambojjhaṅgaratanassa pātubhāvo hoti,",
  "kv1.9:12.3_6": "upekkhāsambojjhaṅgaratanassa pātubhāvo hoti.",
  "kv1.9:12.4_0": "Tathāgatassa, bhikkhave,",
  "kv1.9:12.4_1": "pātubhāvā arahato sammāsambuddhassa imesaṁ sattannaṁ bojjhaṅgaratanānaṁ pātubhāvo hotī”ti.",
  "kv1.9:12.5_0": "Attheva suttantoti?",
  "kv1.9:12.6_0": "Āmantā.",
  "kv1.9:12.7_0": "Tathāgatassa pātubhāvā arahato sammāsambuddhassa sabbe dhammā satisambojjhaṅgaratanāva hontīti?",
  "kv1.9:12.8_0": "Na hevaṁ vattabbe …pe…",
  "kv1.9:12.9_0": "sabbe dhammā satipaṭṭhānāti?",
  "kv1.9:12.10_0": "Āmantā.",
  "kv1.9:12.11_0": "Sabbe dhammā sammappadhānā …",
  "kv1.9:12.12_0": "iddhipādā …",
  "kv1.9:12.13_0": "indriyā …",
  "kv1.9:12.14_0": "balā …",
  "kv1.9:12.15_0": "bojjhaṅgāti?",
  "kv1.9:12.16_0": "Na hevaṁ vattabbe …pe….",
  "kv1.9:12.17_0": "Satipaṭṭhānakathā niṭṭhitā."
}