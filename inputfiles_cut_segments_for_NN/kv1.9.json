{
  "kv1.9:0.1_0": "Kathāvatthu",
  "kv1.9:0.2_0": "Mahāpaṇṇāsaka",
  "kv1.9:0.3_0": "Paṭhamavagga",
  "kv1.9:0.4_0": "Satipaṭṭhānakathā",
  "kv1.9:1.1_0": "Sabbe dhammā satipaṭṭhānāti?",
  "kv1.9:1.2_0": "Āmantā.",
  "kv1.9:1.3_0": "Sabbe dhammā sati satindriyaṁ satibalaṁ sammāsati satisambojjhaṅgo ekāyanamaggo khayagāmī bodhagāmī apacayagāmī anāsavā asaṁyojaniyā aganthaniyā anoghaniyā ayoganiyā anīvaraṇiyā aparāmaṭṭhā anupādāniyā asaṅkilesikā,",
  "kv1.9:1.3_1": "sabbe dhammā buddhānussati dhammānussati saṅghānussati sīlānussati cāgānussati devatānussati ānāpānassati maraṇānussati kāyagatāsati upasamānussatīti?",
  "kv1.9:1.4_0": "Na hevaṁ vattabbe …pe….",
  "kv1.9:2.1_0": "Sabbe dhammā satipaṭṭhānāti?",
  "kv1.9:2.2_0": "Āmantā.",
  "kv1.9:2.3_0": "Cakkhāyatanaṁ satipaṭṭhānanti?",
  "kv1.9:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv1.9:2.5_0": "cakkhāyatanaṁ satipaṭṭhānanti?",
  "kv1.9:2.6_0": "Āmantā.",
  "kv1.9:2.7_0": "Cakkhāyatanaṁ sati satindriyaṁ satibalaṁ sammāsati satisambojjhaṅgo ekāyanamaggo khayagāmī bodhagāmī apacayagāmī anāsavaṁ asaṁyojaniyaṁ …pe… asaṅkilesikaṁ,",
  "kv1.9:2.7_1": "cakkhāyatanaṁ buddhānussati dhammānussati saṅghānussati sīlānussati cāgānussati devatānussati ānāpānassati maraṇānussati kāyagatāsati upasamānussatīti?",
  "kv1.9:2.8_0": "Na hevaṁ vattabbe …pe…",
  "kv1.9:2.9_0": "sotāyatanaṁ …",
