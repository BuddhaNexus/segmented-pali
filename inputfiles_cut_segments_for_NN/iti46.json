{
  "iti46:0.1_0": "Itivuttaka 46",
  "iti46:0.2_0": "Dukanipāta",
  "iti46:0.3_0": "Dutiyavagga",
  "iti46:0.4_0": "Sikkhānisaṁsasutta",
  "iti46:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti46:2.1_0": "“Sikkhānisaṁsā, bhikkhave, viharatha paññuttarā vimuttisārā satādhipateyyā.",
  "iti46:2.2_0": "Sikkhānisaṁsānaṁ,",
  "iti46:2.2_1": "bhikkhave,",
  "iti46:2.2_2": "viharataṁ paññuttarānaṁ vimuttisārānaṁ satādhipateyyānaṁ dvinnaṁ phalānaṁ aññataraṁ phalaṁ pāṭikaṅkhaṁ—",
  "iti46:2.3_0": "diṭṭheva dhamme aññā, sati vā upādisese anāgāmitā”ti.",
  "iti46:2.4_0": "Etamatthaṁ bhagavā avoca.",
  "iti46:2.5_0": "Tatthetaṁ iti vuccati:",
  "iti46:3.1_0": "“Paripuṇṇasikkhaṁ apahānadhammaṁ,",
  "iti46:3.2_0": "Paññuttaraṁ jātikhayantadassiṁ;",
  "iti46:3.3_0": "Taṁ ve muniṁ antimadehadhāriṁ,",
  "iti46:3.4_0": "Mārañjahaṁ brūmi jarāya pāraguṁ.",
  "iti46:4.1_0": "Tasmā sadā jhānaratā samāhitā,",
  "iti46:4.2_0": "Ātāpino jātikhayantadassino;",
  "iti46:4.3_0": "Māraṁ sasenaṁ abhibhuyya bhikkhavo,",
  "iti46:4.4_0": "Bhavatha jātimaraṇassa pāragā”ti.",
  "iti46:5.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti46:5.2_0": "Navamaṁ."
}