{
  "ja77:0.1_0": "Jātaka",
  "ja77:0.2_0": "Ekakanipāta",
  "ja77:0.3_0": "Varuṇavagga",
  "ja77:0.4_0": "7. Mahāsupinajātaka",
  "ja77:1.1_0": "“Usabhā rukkhā gāviyo gavā ca,",
  "ja77:1.2_0": "Asso kaṁso siṅgālī ca kumbho;",
  "ja77:1.3_0": "Pokkharaṇī ca apākacandanaṁ,",
  "ja77:1.4_0": "Lābūni sīdanti silā plavan”ti.",
  "ja77:2.1_0": "Maṇḍūkiyo kaṇhasappe gilanti,",
  "ja77:2.2_0": "Kākaṁ supaṇṇā parivārayanti;",
  "ja77:2.3_0": "Tasā vakā eḷakānaṁ bhayāhi,",
  "ja77:2.4_0": "Vipariyāso vattati nayidha matthīti.",
  "ja77:3.1_0": "Mahāsupinajātakaṁ sattamaṁ."
}