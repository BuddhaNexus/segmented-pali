{
  "kv11.8:0.1_0": "Kathāvatthu",
  "kv11.8:0.2_0": "Mahāpaṇṇāsaka",
  "kv11.8:0.3_0": "Ekādasamavagga",
  "kv11.8:0.4_0": "Aniccatākathā",
  "kv11.8:1.1_0": "Aniccatā parinipphannāti?",
  "kv11.8:1.2_0": "Āmantā.",
  "kv11.8:1.3_0": "Tāya aniccatāya aniccatā parinipphannāti?",
  "kv11.8:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv11.8:1.5_0": "tāya aniccatāya aniccatā parinipphannāti?",
  "kv11.8:1.6_0": "Āmantā.",
  "kv11.8:1.7_0": "Tāya tāyeva natthi dukkhassantakiriyā, natthi vaṭṭupacchedo, natthi anupādāparinibbānanti?",
  "kv11.8:1.8_0": "Na hevaṁ vattabbe.",
  "kv11.8:2.1_0": "Jarā parinipphannāti?",
  "kv11.8:2.2_0": "Āmantā.",
  "kv11.8:2.3_0": "Tāya jarāya jarā parinipphannāti?",
  "kv11.8:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv11.8:2.5_0": "tāya jarāya jarā parinipphannāti?",
  "kv11.8:2.6_0": "Āmantā.",
  "kv11.8:2.7_0": "Tāya tāyeva natthi dukkhassantakiriyā, natthi vaṭṭupacchedo, natthi anupādāparinibbānanti?",
  "kv11.8:2.8_0": "Na hevaṁ vattabbe …pe….",
  "kv11.8:3.1_0": "Maraṇaṁ parinipphannanti?",
  "kv11.8:3.2_0": "Āmantā.",
  "kv11.8:3.3_0": "Tassa maraṇassa maraṇaṁ parinipphannanti?",
  "kv11.8:3.4_0": "Na hevaṁ vattabbe.",
  "kv11.8:3.5_0": "Tassa maraṇassa maraṇaṁ parinipphannanti?",
  "kv11.8:3.6_0": "Āmantā.",
  "kv11.8:3.7_0": "Tassa tasseva natthi dukkhassantakiriyā, natthi vaṭṭupacchedo, natthi anupādāparinibbānanti?",
  "kv11.8:3.8_0": "Na hevaṁ vattabbe …pe….",
  "kv11.8:4.1_0": "Rūpaṁ parinipphannaṁ, rūpassa aniccatā atthīti?",
  "kv11.8:4.2_0": "Āmantā.",
  "kv11.8:4.3_0": "Aniccatā parinipphannā, aniccatāya aniccatā atthīti?",
  "kv11.8:4.4_0": "Na hevaṁ vattabbe …pe…",
  "kv11.8:4.5_0": "rūpaṁ parinipphannaṁ, rūpassa jarā atthīti?",
  "kv11.8:4.6_0": "Āmantā.",
  "kv11.8:4.7_0": "Jarā parinipphannā, jarāya jarā atthīti?",
  "kv11.8:4.8_0": "Na hevaṁ vattabbe …pe….",
  "kv11.8:5.1_0": "Rūpaṁ parinipphannaṁ, rūpassa bhedo atthi, antaradhānaṁ atthīti?",
  "kv11.8:5.2_0": "Āmantā.",
  "kv11.8:5.3_0": "Maraṇaṁ parinipphannaṁ, maraṇassa bhedo atthi, antaradhānaṁ atthīti?",
  "kv11.8:5.4_0": "Na hevaṁ vattabbe …pe….",
  "kv11.8:6.1_0": "Vedanā …pe…",
  "kv11.8:6.2_0": "saññā … saṅkhārā …pe…",
  "kv11.8:6.3_0": "viññāṇaṁ parinipphannaṁ, viññāṇassa aniccatā atthīti?",
  "kv11.8:6.4_0": "Āmantā.",
  "kv11.8:6.5_0": "Aniccatā parinipphannā, aniccatāya aniccatā atthīti?",
  "kv11.8:6.6_0": "Na hevaṁ vattabbe …pe…",
  "kv11.8:6.7_0": "viññāṇaṁ parinipphannaṁ, viññāṇassa jarā atthīti?",
  "kv11.8:6.8_0": "Āmantā.",
  "kv11.8:6.9_0": "Jarā parinipphannā, jarāya jarā atthīti?",
  "kv11.8:6.10_0": "Na hevaṁ vattabbe …pe….",
  "kv11.8:7.1_0": "Viññāṇaṁ parinipphannaṁ, viññāṇassa bhedo atthi, antaradhānaṁ atthīti?",
  "kv11.8:7.2_0": "Āmantā.",
  "kv11.8:7.3_0": "Maraṇaṁ parinipphannaṁ, maraṇassa bhedo atthi, antaradhānaṁ atthīti?",
  "kv11.8:7.4_0": "Na hevaṁ vattabbe …pe….",
  "kv11.8:7.5_0": "Aniccatākathā niṭṭhitā.",
  "kv11.8:7.6_0": "Ekādasamo vaggo.",
  "kv11.8:7.7_0": "Tassuddānaṁ",
  "kv11.8:8.1_0": "Anusayā abyākatā, ahetukā, cittavippayuttā, aññāṇe vigate ñāṇī, ñāṇaṁ cittavippayuttaṁ,",
  "kv11.8:8.1_1": "yattha sadde ñāṇaṁ pavattati,",
  "kv11.8:8.1_2": "iddhibalena samannāgato kappaṁ tiṭṭheyya, cittasantati samādhi, dhammaṭṭhitatā, aniccatāti."
}