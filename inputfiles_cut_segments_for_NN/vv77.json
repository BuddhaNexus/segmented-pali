{
  "vv77:0.1_0": "Vimānavatthu",
  "vv77:0.2_0": "Purisavimāna",
  "vv77:0.3_0": "Sunikkhittavagga",
  "vv77:0.4_0": "3. Maṇithūṇavimānavatthu",
  "vv77:1.1_0": "“Uccamidaṁ maṇithūṇaṁ vimānaṁ,",
  "vv77:1.2_0": "Samantato dvādasa yojanāni;",
  "vv77:1.3_0": "Kūṭāgārā sattasatā uḷārā,",
  "vv77:1.4_0": "Veḷuriyathambhā rucakatthatā subhā.",
  "vv77:2.1_0": "Tatthacchasi pivasi khādasi ca,",
  "vv77:2.2_0": "Dibbā ca vīṇā pavadanti vagguṁ;",
  "vv77:2.3_0": "Dibbā rasā kāmaguṇettha pañca,",
  "vv77:2.4_0": "Nāriyo ca naccanti suvaṇṇachannā.",
  "vv77:3.1_0": "Kena tetādiso vaṇṇo,",
  "vv77:3.2_0": "…pe…",
  "vv77:3.3_0": "vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv77:4.1_0": "So devaputto attamano,",
  "vv77:4.2_0": "…pe…",
  "vv77:4.3_0": "yassa kammassidaṁ phalaṁ.",
  "vv77:5.1_0": "“Ahaṁ manussesu manussabhūto,",
  "vv77:5.2_0": "Vivane pathe saṅkamanaṁ akāsiṁ;",
  "vv77:5.3_0": "Ārāmarukkhāni ca ropayissaṁ,",
  "vv77:5.4_0": "Piyā ca me sīlavanto ahesuṁ;",
  "vv77:5.5_0": "Annañca pānañca pasannacitto,",
  "vv77:5.6_0": "Sakkacca dānaṁ vipulaṁ adāsiṁ.",
  "vv77:6.1_0": "Tena metādiso vaṇṇo,",
  "vv77:6.2_0": "…pe…",
  "vv77:6.3_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv77:7.1_0": "Maṇithūṇavimānaṁ tatiyaṁ."
}