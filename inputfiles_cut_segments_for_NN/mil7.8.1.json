{
  "mil7.8.1:0.1_0": "Milindapañha",
  "mil7.8.1:0.2_0": "Opammakathāpañha",
  "mil7.8.1:0.3_0": "Kumbhavagga",
  "mil7.8.1:0.4_0": "1. Kumbhaṅgapañha",
  "mil7.8.1:1.1_0": "“Bhante nāgasena,",
  "mil7.8.1:1.1_1": "‘kumbhassa ekaṁ aṅgaṁ gahetabban’ti yaṁ vadesi,",
  "mil7.8.1:1.1_2": "katamaṁ taṁ ekaṁ aṅgaṁ gahetabban”ti?",
  "mil7.8.1:1.2_0": "“Yathā, mahārāja, kumbho sampuṇṇo na saṇati;",
  "mil7.8.1:1.3_0": "evameva kho, mahārāja,",
  "mil7.8.1:1.3_1": "yoginā yogāvacarena āgame adhigame pariyattiyaṁ sāmaññe pāramiṁ patvā na saṇitabbaṁ,",
  "mil7.8.1:1.3_2": "na tena māno karaṇīyo,",
  "mil7.8.1:1.3_3": "na dabbo dassetabbo,",
  "mil7.8.1:1.3_4": "nihatamānena nihatadabbena bhavitabbaṁ,",
  "mil7.8.1:1.3_5": "ujukena amukharena avikatthinā.",
  "mil7.8.1:1.4_0": "Idaṁ, mahārāja, kumbhassa ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.8.1:1.5_0": "Bhāsitampetaṁ, mahārāja, bhagavatā devātidevena suttanipāte—",
  "mil7.8.1:2.1_0": "‘Yadūnakaṁ taṁ saṇati,",
  "mil7.8.1:2.2_0": "Yaṁ pūraṁ santameva taṁ;",
  "mil7.8.1:2.3_0": "Aḍḍhakumbhūpamo bālo,",
  "mil7.8.1:2.4_0": "Rahado pūrova paṇḍito’”ti.",
  "mil7.8.1:3.1_0": "Kumbhaṅgapañho paṭhamo."
}