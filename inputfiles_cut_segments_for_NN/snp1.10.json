{
  "snp1.10:0.1_0": "Sutta Nipāta 1.10",
  "snp1.10:0.2_0": "Āḷavakasutta",
  "snp1.10:1.1_0": "Evaṁ me sutaṁ—",
  "snp1.10:1.2_0": " ekaṁ samayaṁ bhagavā āḷaviyaṁ viharati āḷavakassa yakkhassa bhavane.",
  "snp1.10:1.3_0": "Atha kho āḷavako yakkho yena bhagavā tenupasaṅkami;",
  "snp1.10:1.3_1": "upasaṅkamitvā bhagavantaṁ etadavoca:",
  "snp1.10:1.4_0": "“nikkhama, samaṇā”ti.",
  "snp1.10:1.5_0": "“Sādhāvuso”ti bhagavā nikkhami.",
  "snp1.10:1.6_0": "“Pavisa, samaṇā”ti.",
  "snp1.10:1.7_0": "“Sādhāvuso”ti bhagavā pāvisi.",
  "snp1.10:2.1_0": "Dutiyampi kho …pe…",
  "snp1.10:2.2_0": "tatiyampi kho āḷavako yakkho bhagavantaṁ etadavoca:",
  "snp1.10:2.3_0": "“nikkhama, samaṇā”ti.",
  "snp1.10:2.4_0": "“Sādhāvuso”ti bhagavā nikkhami.",
  "snp1.10:2.5_0": "“Pavisa, samaṇā”ti.",
  "snp1.10:2.6_0": "“Sādhāvuso”ti bhagavā pāvisi.",
  "snp1.10:3.1_0": "Catutthampi kho āḷavako yakkho bhagavantaṁ etadavoca:",
  "snp1.10:3.2_0": "“nikkhama, samaṇā”ti.",
  "snp1.10:3.3_0": "“Na khvāhaṁ taṁ, āvuso, nikkhamissāmi.",
  "snp1.10:3.4_0": "Yaṁ te karaṇīyaṁ, taṁ karohī”ti.",
  "snp1.10:4.1_0": "“Pañhaṁ taṁ, samaṇa, pucchissāmi.",
  "snp1.10:4.2_0": "Sace me na byākarissasi, cittaṁ vā te khipissāmi, hadayaṁ vā te phālessāmi,",
  "snp1.10:4.2_1": "pādesu vā gahetvā pāragaṅgāya khipissāmī”ti.",
  "snp1.10:5.1_0": "“Na khvāhaṁ taṁ, āvuso,",
  "snp1.10:5.1_1": "passāmi sadevake loke samārake sabrahmake sassamaṇabrāhmaṇiyā pajāya sadevamanussāya yo me cittaṁ vā khipeyya hadayaṁ vā phāleyya pādesu vā gahetvā pāragaṅgāya khipeyya.",
  "snp1.10:5.2_0": "Api ca tvaṁ, āvuso, puccha yadākaṅkhasī”ti.",
  "snp1.10:5.3_0": "Atha kho āḷavako yakkho bhagavantaṁ gāthāya ajjhabhāsi:",
  "snp1.10:6.1_0": "“Kiṁ sūdha vittaṁ purisassa seṭṭhaṁ,",
  "snp1.10:6.2_0": "Kiṁ su suciṇṇaṁ sukhamāvahāti;",
  "snp1.10:6.3_0": "Kiṁ su have sādutaraṁ rasānaṁ,",
  "snp1.10:6.4_0": "Kathaṁjīviṁ jīvitamāhu seṭṭhaṁ”.",
  "snp1.10:7.1_0": "“Saddhīdha vittaṁ purisassa seṭṭhaṁ,",
  "snp1.10:7.2_0": "Dhammo suciṇṇo sukhamāvahāti;",
  "snp1.10:7.3_0": "Saccaṁ have sādutaraṁ rasānaṁ,",
  "snp1.10:7.4_0": "Paññājīviṁ jīvitamāhu seṭṭhaṁ”.",
  "snp1.10:8.1_0": "“Kathaṁ su tarati oghaṁ,",
  "snp1.10:8.2_0": "kathaṁ su tarati aṇṇavaṁ;",
  "snp1.10:8.3_0": "Kathaṁ su dukkhamacceti,",
  "snp1.10:8.4_0": "kathaṁ su parisujjhati”.",
  "snp1.10:9.1_0": "“Saddhāya tarati oghaṁ,",
  "snp1.10:9.2_0": "appamādena aṇṇavaṁ;",
  "snp1.10:9.3_0": "Vīriyena dukkhamacceti,",
  "snp1.10:9.4_0": "paññāya parisujjhati”.",
  "snp1.10:10.1_0": "“Kathaṁ su labhate paññaṁ,",
  "snp1.10:10.2_0": "kathaṁ su vindate dhanaṁ;",
  "snp1.10:10.3_0": "Kathaṁ su kittiṁ pappoti,",
  "snp1.10:10.4_0": "kathaṁ mittāni ganthati;",
  "snp1.10:10.5_0": "Asmā lokā paraṁ lokaṁ,",
  "snp1.10:10.6_0": "kathaṁ pecca na socati”.",
  "snp1.10:11.1_0": "“Saddahāno arahataṁ,",
  "snp1.10:11.2_0": "dhammaṁ nibbānapattiyā;",
  "snp1.10:11.3_0": "Sussūsaṁ labhate paññaṁ,",
  "snp1.10:11.4_0": "appamatto vicakkhaṇo.",
  "snp1.10:12.1_0": "Patirūpakārī dhuravā,",
  "snp1.10:12.2_0": "uṭṭhātā vindate dhanaṁ;",
  "snp1.10:12.3_0": "Saccena kittiṁ pappoti,",
  "snp1.10:12.4_0": "dadaṁ mittāni ganthati.",
  "snp1.10:13.1_0": "Yassete caturo dhammā,",
  "snp1.10:13.2_0": "saddhassa gharamesino;",
  "snp1.10:13.3_0": "Saccaṁ dhammo dhiti cāgo,",
  "snp1.10:13.4_0": "sa ve pecca na socati.",
  "snp1.10:14.1_0": "Iṅgha aññepi pucchassu,",
  "snp1.10:14.2_0": "puthū samaṇabrāhmaṇe;",
  "snp1.10:14.3_0": "Yadi saccā damā cāgā,",
  "snp1.10:14.4_0": "khantyā bhiyyodha vijjati”.",
  "snp1.10:15.1_0": "“Kathaṁ nu dāni puccheyyaṁ,",
  "snp1.10:15.2_0": "Puthū samaṇabrāhmaṇe;",
  "snp1.10:15.3_0": "Yohaṁ ajja pajānāmi,",
  "snp1.10:15.4_0": "Yo attho samparāyiko.",
  "snp1.10:16.1_0": "Atthāya vata me buddho,",
  "snp1.10:16.2_0": "Vāsāyāḷavimāgamā;",
  "snp1.10:16.3_0": "Yohaṁ ajja pajānāmi,",
  "snp1.10:16.4_0": "Yattha dinnaṁ mahapphalaṁ.",
  "snp1.10:17.1_0": "So ahaṁ vicarissāmi,",
  "snp1.10:17.2_0": "Gāmā gāmaṁ purā puraṁ;",
  "snp1.10:17.3_0": "Namassamāno sambuddhaṁ,",
  "snp1.10:17.4_0": "Dhammassa ca sudhammatan”ti.",
  "snp1.10:17.5_0": "Āḷavakasuttaṁ dasamaṁ."
}