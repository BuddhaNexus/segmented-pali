{
  "tha-ap115:0.1_0": "Therāpadāna",
  "tha-ap115:0.2_0": "Mahāparivāravagga",
  "tha-ap115:0.3_0": "3 Saraṇagamaniyattheraapadāna",
  "tha-ap115:1.1_0": "“Ubhinnaṁ devarājūnaṁ,",
  "tha-ap115:1.2_0": "saṅgāmo samupaṭṭhito;",
  "tha-ap115:1.3_0": "Ahosi samupabyūḷho,",
  "tha-ap115:1.4_0": "mahāghoso avattatha.",
  "tha-ap115:2.1_0": "Padumuttaro lokavidū,",
  "tha-ap115:2.2_0": "āhutīnaṁ paṭiggaho;",
  "tha-ap115:2.3_0": "Antalikkhe ṭhito satthā,",
  "tha-ap115:2.4_0": "saṁvejesi mahājanaṁ.",
  "tha-ap115:3.1_0": "Sabbe devā attamanā,",
  "tha-ap115:3.2_0": "nikkhittakavacāvudhā;",
  "tha-ap115:3.3_0": "Sambuddhaṁ abhivādetvā,",
  "tha-ap115:3.4_0": "ekaggāsiṁsu tāvade.",
  "tha-ap115:4.1_0": "Mayhaṁ saṅkappamaññāya,",
  "tha-ap115:4.2_0": "vācāsabhimudīrayi;",
  "tha-ap115:4.3_0": "Anukampako lokavidū,",
  "tha-ap115:4.4_0": "nibbāpesi mahājanaṁ.",
  "tha-ap115:5.1_0": "Paduṭṭhacitto manujo,",
  "tha-ap115:5.2_0": "ekapāṇaṁ viheṭhayaṁ;",
  "tha-ap115:5.3_0": "Tena cittappadosena,",
  "tha-ap115:5.4_0": "apāyaṁ upapajjati.",
  "tha-ap115:6.1_0": "Saṅgāmasīse nāgova,",
  "tha-ap115:6.2_0": "bahū pāṇe viheṭhayaṁ;",
  "tha-ap115:6.3_0": "Nibbāpetha sakaṁ cittaṁ,",
  "tha-ap115:6.4_0": "mā haññittho punappunaṁ.",
  "tha-ap115:7.1_0": "Dvinnampi yakkharājūnaṁ,",
  "tha-ap115:7.2_0": "senā sā vimhitā ahu;",
  "tha-ap115:7.3_0": "Saraṇañca upāgacchuṁ,",
  "tha-ap115:7.4_0": "lokajeṭṭhaṁ sutādinaṁ.",
  "tha-ap115:8.1_0": "Saññāpetvāna janataṁ,",
  "tha-ap115:8.2_0": "padamuddhari cakkhumā;",
  "tha-ap115:8.3_0": "Pekkhamānova devehi,",
  "tha-ap115:8.4_0": "pakkāmi uttarāmukho.",
  "tha-ap115:9.1_0": "Paṭhamaṁ saraṇaṁ gacchiṁ,",
  "tha-ap115:9.2_0": "dvipadindassa tādino;",
  "tha-ap115:9.3_0": "Kappānaṁ satasahassaṁ,",
  "tha-ap115:9.4_0": "duggatiṁ nupapajjahaṁ.",
  "tha-ap115:10.1_0": "Mahādundubhināmā ca,",
  "tha-ap115:10.2_0": "soḷasāsuṁ rathesabhā;",
  "tha-ap115:10.3_0": "Tiṁsakappasahassamhi,",
  "tha-ap115:10.4_0": "rājāno cakkavattino.",
  "tha-ap115:11.1_0": "Paṭisambhidā catasso,",
  "tha-ap115:11.2_0": "…pe…",
  "tha-ap115:11.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap115:12.1_0": "Itthaṁ sudaṁ āyasmā saraṇagamaniyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap115:13.1_0": "Saraṇagamaniyattherassāpadānaṁ tatiyaṁ."
}