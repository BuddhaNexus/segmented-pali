{
  "tha-ap394:0.1_0": "Therāpadāna",
  "tha-ap394:0.2_0": "Pilindavacchavagga",
  "tha-ap394:0.3_0": "2. Selattheraapadāna",
  "tha-ap394:1.1_0": "“Nagare haṁsavatiyā,",
  "tha-ap394:1.2_0": "vīthisāmī ahosahaṁ;",
  "tha-ap394:1.3_0": "Mama ñātī samānetvā,",
  "tha-ap394:1.4_0": "idaṁ vacanamabraviṁ.",
  "tha-ap394:2.1_0": "‘Buddho loke samuppanno,",
  "tha-ap394:2.2_0": "puññakkhetto anuttaro;",
  "tha-ap394:2.3_0": "Āsi so sabbalokassa,",
  "tha-ap394:2.4_0": "āhutīnaṁ paṭiggaho.",
  "tha-ap394:3.1_0": "Khattiyā negamā ceva,",
  "tha-ap394:3.2_0": "mahāsālā ca brāhmaṇā;",
  "tha-ap394:3.3_0": "Pasannacittā sumanā,",
  "tha-ap394:3.4_0": "pūgadhammaṁ akaṁsu te.",
  "tha-ap394:4.1_0": "Hatthārohā anīkaṭṭhā,",
  "tha-ap394:4.2_0": "rathikā pattikārakā;",
  "tha-ap394:4.3_0": "Pasannacittā sumanā,",
  "tha-ap394:4.4_0": "pūgadhammaṁ akaṁsu te.",
  "tha-ap394:5.1_0": "Uggā ca rājaputtā ca,",
  "tha-ap394:5.2_0": "vesiyānā ca brāhmaṇā;",
  "tha-ap394:5.3_0": "Pasannacittā sumanā,",
  "tha-ap394:5.4_0": "pūgadhammaṁ akaṁsu te.",
  "tha-ap394:6.1_0": "Āḷārikā kappakā ca,",
  "tha-ap394:6.2_0": "nhāpakā mālakārakā;",
  "tha-ap394:6.3_0": "Pasannacittā sumanā,",
  "tha-ap394:6.4_0": "pūgadhammaṁ akaṁsu te.",
  "tha-ap394:7.1_0": "Rajakā pesakārā ca,",
  "tha-ap394:7.2_0": "cammakārā ca nhāpitā;",
  "tha-ap394:7.3_0": "Pasannacittā sumanā,",
  "tha-ap394:7.4_0": "pūgadhammaṁ akaṁsu te.",
  "tha-ap394:8.1_0": "Usukārā bhamakārā,",
  "tha-ap394:8.2_0": "cammakārā ca tacchakā;",
  "tha-ap394:8.3_0": "Pasannacittā sumanā,",
  "tha-ap394:8.4_0": "pūgadhammaṁ akaṁsu te.",
  "tha-ap394:9.1_0": "Kammārā soṇṇakārā ca,",
  "tha-ap394:9.2_0": "tipulohakarā tathā;",
  "tha-ap394:9.3_0": "Pasannacittā sumanā,",
  "tha-ap394:9.4_0": "pūgadhammaṁ akaṁsu te.",
  "tha-ap394:10.1_0": "Bhatakā ceṭakā ceva,",
  "tha-ap394:10.2_0": "dāsakammakarā bahū;",
  "tha-ap394:10.3_0": "Yathāsakena thāmena,",
  "tha-ap394:10.4_0": "pūgadhammaṁ akaṁsu te.",
  "tha-ap394:11.1_0": "Udahārā kaṭṭhahārā,",
  "tha-ap394:11.2_0": "kassakā tiṇahārakā;",
  "tha-ap394:11.3_0": "Yathāsakena thāmena,",
  "tha-ap394:11.4_0": "pūgadhammaṁ akaṁsu te.",
  "tha-ap394:12.1_0": "Pupphikā mālikā ceva,",
  "tha-ap394:12.2_0": "paṇṇikā phalahārakā;",
  "tha-ap394:12.3_0": "Yathāsakena thāmena,",
  "tha-ap394:12.4_0": "pūgadhammaṁ akaṁsu te.",
  "tha-ap394:13.1_0": "Gaṇikā kumbhadāsī ca,",
  "tha-ap394:13.2_0": "pūvikā macchikāpi ca;",
  "tha-ap394:13.3_0": "Yathāsakena thāmena,",
  "tha-ap394:13.4_0": "pūgadhammaṁ akaṁsu te.",
  "tha-ap394:14.1_0": "Etha sabbe samāgantvā,",
  "tha-ap394:14.2_0": "gaṇaṁ bandhāma ekato;",
  "tha-ap394:14.3_0": "Adhikāraṁ karissāma,",
  "tha-ap394:14.4_0": "puññakkhette anuttare’.",
  "tha-ap394:15.1_0": "Te me sutvāna vacanaṁ,",
  "tha-ap394:15.2_0": "gaṇaṁ bandhiṁsu tāvade;",
  "tha-ap394:15.3_0": "‘Upaṭṭhānasālaṁ sukataṁ,",
  "tha-ap394:15.4_0": "bhikkhusaṅghassa kārayuṁ’.",
  "tha-ap394:16.1_0": "Niṭṭhāpetvāna taṁ sālaṁ,",
  "tha-ap394:16.2_0": "udaggo tuṭṭhamānaso;",
  "tha-ap394:16.3_0": "Pareto tehi sabbehi,",
  "tha-ap394:16.4_0": "sambuddhamupasaṅkamiṁ.",
  "tha-ap394:17.1_0": "Upasaṅkamma sambuddhaṁ,",
  "tha-ap394:17.2_0": "lokanāthaṁ narāsabhaṁ;",
  "tha-ap394:17.3_0": "Vanditvā satthuno pāde,",
  "tha-ap394:17.4_0": "idaṁ vacanamabraviṁ.",
  "tha-ap394:18.1_0": "‘Ime tīṇi satā vīra,",
  "tha-ap394:18.2_0": "Purisā ekato gaṇā;",
  "tha-ap394:18.3_0": "Upaṭṭhānasālaṁ sukataṁ,",
  "tha-ap394:18.4_0": "Niyyādenti tuvaṁ muni.",
  "tha-ap394:19.1_0": "Bhikkhusaṅghassa purato,",
  "tha-ap394:19.2_0": "sampaṭicchitva cakkhumā’;",
  "tha-ap394:19.3_0": "Tiṇṇaṁ satānaṁ purato,",
  "tha-ap394:19.4_0": "imā gāthā abhāsatha.",
  "tha-ap394:20.1_0": "‘Tisatāpi ca jeṭṭho ca,",
  "tha-ap394:20.2_0": "anuvattiṁsu ekato;",
  "tha-ap394:20.3_0": "Sampattiñhi karitvāna,",
  "tha-ap394:20.4_0": "sabbe anubhavissatha.",
  "tha-ap394:21.1_0": "Pacchime bhave sampatte,",
  "tha-ap394:21.2_0": "sītibhāvamanuttaraṁ;",
  "tha-ap394:21.3_0": "Ajaraṁ amataṁ santaṁ,",
  "tha-ap394:21.4_0": "nibbānaṁ phassayissatha’.",
  "tha-ap394:22.1_0": "Evaṁ buddho viyākāsi,",
  "tha-ap394:22.2_0": "sabbaññū samaṇuttaro;",
  "tha-ap394:22.3_0": "Buddhassa vacanaṁ sutvā,",
  "tha-ap394:22.4_0": "somanassaṁ pavedayiṁ.",
  "tha-ap394:23.1_0": "Tiṁsa kappasahassāni,",
  "tha-ap394:23.2_0": "devaloke ramiṁ ahaṁ;",
  "tha-ap394:23.3_0": "Devādhipo pañcasataṁ,",
  "tha-ap394:23.4_0": "devarajjamakārayiṁ.",
  "tha-ap394:24.1_0": "Sahassakkhattuṁ rājā ca,",
  "tha-ap394:24.2_0": "cakkavattī ahosahaṁ;",
  "tha-ap394:24.3_0": "Devarajjaṁ karontassa,",
  "tha-ap394:24.4_0": "mahādevā avandisuṁ.",
  "tha-ap394:25.1_0": "Idha mānusake rajjaṁ,",
  "tha-ap394:25.2_0": "parisā honti bandhavā;",
  "tha-ap394:25.3_0": "Pacchime bhave sampatte,",
  "tha-ap394:25.4_0": "vāseṭṭho nāma brāhmaṇo.",
  "tha-ap394:26.1_0": "Asītikoṭi nicayo,",
  "tha-ap394:26.2_0": "tassa putto ahosahaṁ;",
  "tha-ap394:26.3_0": "Selo iti mamaṁ nāmaṁ,",
  "tha-ap394:26.4_0": "chaḷaṅge pāramiṁ gato.",
  "tha-ap394:27.1_0": "Jaṅghāvihāraṁ vicaraṁ,",
  "tha-ap394:27.2_0": "sasissehi purakkhato;",
  "tha-ap394:27.3_0": "Jaṭābhārikabharitaṁ,",
  "tha-ap394:27.4_0": "keṇiyaṁ nāma tāpasaṁ.",
  "tha-ap394:28.1_0": "Paṭiyattāhutiṁ disvā,",
  "tha-ap394:28.2_0": "idaṁ vacanamabraviṁ;",
  "tha-ap394:28.3_0": "‘Āvāho vā vivāho vā,",
  "tha-ap394:28.4_0": "rājā vā te nimantito’.",
  "tha-ap394:29.1_0": "‘Āhutiṁ yiṭṭhukāmohaṁ,",
  "tha-ap394:29.2_0": "brāhmaṇe devasammate;",
  "tha-ap394:29.3_0": "Na nimantemi rājānaṁ,",
  "tha-ap394:29.4_0": "āhutī me na vijjati.",
  "tha-ap394:30.1_0": "Na catthi mayhamāvāho,",
  "tha-ap394:30.2_0": "vivāho me na vijjati;",
  "tha-ap394:30.3_0": "Sakyānaṁ nandijanano,",
  "tha-ap394:30.4_0": "seṭṭho loke sadevake.",
  "tha-ap394:31.1_0": "Sabbalokahitatthāya,",
  "tha-ap394:31.2_0": "sabbasattasukhāvaho;",
  "tha-ap394:31.3_0": "So me nimantito ajja,",
  "tha-ap394:31.4_0": "tassetaṁ paṭiyādanaṁ.",
  "tha-ap394:32.1_0": "Timbarūsakavaṇṇābho,",
  "tha-ap394:32.2_0": "appameyyo anūpamo;",
  "tha-ap394:32.3_0": "Rūpenāsadiso buddho,",
  "tha-ap394:32.4_0": "svātanāya nimantito.",
  "tha-ap394:33.1_0": "Ukkāmukhapahaṭṭhova,",
  "tha-ap394:33.2_0": "khadiraṅgārasannibho;",
  "tha-ap394:33.3_0": "Vijjūpamo mahāvīro,",
  "tha-ap394:33.4_0": "so me buddho nimantito.",
  "tha-ap394:34.1_0": "Pabbatagge yathā acci,",
  "tha-ap394:34.2_0": "puṇṇamāyeva candimā;",
  "tha-ap394:34.3_0": "Naḷaggivaṇṇasaṅkāso,",
  "tha-ap394:34.4_0": "so me buddho nimantito.",
  "tha-ap394:35.1_0": "Asambhīto bhayātīto,",
  "tha-ap394:35.2_0": "bhavantakaraṇo muni;",
  "tha-ap394:35.3_0": "Sīhūpamo mahāvīro,",
  "tha-ap394:35.4_0": "so me buddho nimantito.",
  "tha-ap394:36.1_0": "Kusalo buddhadhammehi,",
  "tha-ap394:36.2_0": "apasayho parehi so;",
  "tha-ap394:36.3_0": "Nāgūpamo mahāvīro,",
  "tha-ap394:36.4_0": "so me buddho nimantito.",
  "tha-ap394:37.1_0": "Saddhammācārakusalo,",
  "tha-ap394:37.2_0": "buddhanāgo asādiso;",
  "tha-ap394:37.3_0": "Usabhūpamo mahāvīro,",
  "tha-ap394:37.4_0": "so me buddho nimantito.",
  "tha-ap394:38.1_0": "Anantavaṇṇo amitayaso,",
  "tha-ap394:38.2_0": "Vicittasabbalakkhaṇo;",
  "tha-ap394:38.3_0": "Sakkūpamo mahāvīro,",
  "tha-ap394:38.4_0": "So me buddho nimantito.",
  "tha-ap394:39.1_0": "Vasī gaṇī patāpī ca,",
  "tha-ap394:39.2_0": "tejassī ca durāsado;",
  "tha-ap394:39.3_0": "Brahmūpamo mahāvīro,",
  "tha-ap394:39.4_0": "so me buddho nimantito.",
  "tha-ap394:40.1_0": "Pattadhammo dasabalo,",
  "tha-ap394:40.2_0": "balātibalapārago;",
  "tha-ap394:40.3_0": "Dharaṇūpamo mahāvīro,",
  "tha-ap394:40.4_0": "so me buddho nimantito.",
  "tha-ap394:41.1_0": "Sīlavīcisamākiṇṇo,",
  "tha-ap394:41.2_0": "dhammaviññāṇakhobhito;",
  "tha-ap394:41.3_0": "Udadhūpamo mahāvīro,",
  "tha-ap394:41.4_0": "so me buddho nimantito.",
  "tha-ap394:42.1_0": "Durāsado duppasaho,",
  "tha-ap394:42.2_0": "acalo uggato brahā;",
  "tha-ap394:42.3_0": "Nerūpamo mahāvīro,",
  "tha-ap394:42.4_0": "so me buddho nimantito.",
  "tha-ap394:43.1_0": "Anantañāṇo asamasamo,",
  "tha-ap394:43.2_0": "Atulo aggataṁ gato;",
  "tha-ap394:43.3_0": "Gaganūpamo mahāvīro,",
  "tha-ap394:43.4_0": "So me buddho nimantito.",
  "tha-ap394:44.1_0": "Pannarasamaṁ bhāṇavāraṁ.",
  "tha-ap394:45.1_0": "Patiṭṭhā bhayabhītānaṁ,",
  "tha-ap394:45.2_0": "tāṇo saraṇagāminaṁ;",
  "tha-ap394:45.3_0": "Assāsako mahāvīro,",
  "tha-ap394:45.4_0": "so me buddho nimantito.",
  "tha-ap394:46.1_0": "Āsayo buddhimantānaṁ,",
  "tha-ap394:46.2_0": "puññakkhettaṁ sukhesinaṁ;",
  "tha-ap394:46.3_0": "Ratanākaro mahāvīro,",
  "tha-ap394:46.4_0": "so me buddho nimantito.",
  "tha-ap394:47.1_0": "Assāsako vedakaro,",
  "tha-ap394:47.2_0": "sāmaññaphaladāyako;",
  "tha-ap394:47.3_0": "Meghūpamo mahāvīro,",
  "tha-ap394:47.4_0": "so me buddho nimantito.",
  "tha-ap394:48.1_0": "Lokacakkhu mahātejo,",
  "tha-ap394:48.2_0": "sabbatamavinodano;",
  "tha-ap394:48.3_0": "Sūriyūpamo mahāvīro,",
  "tha-ap394:48.4_0": "so me buddho nimantito.",
  "tha-ap394:49.1_0": "Ārammaṇavimuttīsu,",
  "tha-ap394:49.2_0": "sabhāvadassano muni;",
  "tha-ap394:49.3_0": "Candūpamo mahāvīro,",
  "tha-ap394:49.4_0": "so me buddho nimantito.",
  "tha-ap394:50.1_0": "Buddho samussito loke,",
  "tha-ap394:50.2_0": "lakkhaṇehi alaṅkato;",
  "tha-ap394:50.3_0": "Appameyyo mahāvīro,",
  "tha-ap394:50.4_0": "so me buddho nimantito.",
  "tha-ap394:51.1_0": "Yassa ñāṇaṁ appameyyaṁ,",
  "tha-ap394:51.2_0": "sīlaṁ yassa anūpamaṁ;",
  "tha-ap394:51.3_0": "Vimutti asadisā yassa,",
  "tha-ap394:51.4_0": "so me buddho nimantito.",
  "tha-ap394:52.1_0": "Yassa dhīti asadisā,",
  "tha-ap394:52.2_0": "thāmo yassa acintiyo;",
  "tha-ap394:52.3_0": "Yassa parakkamo jeṭṭho,",
  "tha-ap394:52.4_0": "so me buddho nimantito.",
  "tha-ap394:53.1_0": "Rāgo doso ca moho ca,",
  "tha-ap394:53.2_0": "visā sabbe samūhatā;",
  "tha-ap394:53.3_0": "Agadūpamo mahāvīro,",
  "tha-ap394:53.4_0": "so me buddho nimantito.",
  "tha-ap394:54.1_0": "Klesabyādhibahudukkha,",
  "tha-ap394:54.2_0": "sabbatamavinodano;",
  "tha-ap394:54.3_0": "Vejjūpamo mahāvīro,",
  "tha-ap394:54.4_0": "so me buddho nimantito’.",
  "tha-ap394:55.1_0": "Buddhoti bho yaṁ vadesi,",
  "tha-ap394:55.2_0": "ghosopeso sudullabho;",
  "tha-ap394:55.3_0": "Buddho buddhoti sutvāna,",
  "tha-ap394:55.4_0": "pīti me udapajjatha.",
  "tha-ap394:56.1_0": "Abbhantaraṁ agaṇhantaṁ,",
  "tha-ap394:56.2_0": "pīti me bahi nicchare;",
  "tha-ap394:56.3_0": "Sohaṁ pītimano santo,",
  "tha-ap394:56.4_0": "idaṁ vacanamabraviṁ.",
  "tha-ap394:57.1_0": "‘Kahaṁ nu kho so bhagavā,",
  "tha-ap394:57.2_0": "lokajeṭṭho narāsabho;",
  "tha-ap394:57.3_0": "Tattha gantvā namassissaṁ,",
  "tha-ap394:57.4_0": "sāmaññaphaladāyakaṁ.",
  "tha-ap394:58.1_0": "Paggayha dakkhiṇaṁ bāhuṁ,",
  "tha-ap394:58.2_0": "vedajāto katañjalī;",
  "tha-ap394:58.3_0": "Ācikkhi me dhammarājaṁ,",
  "tha-ap394:58.4_0": "sokasallavinodanaṁ’.",
  "tha-ap394:59.1_0": "‘Udentaṁva mahāmeghaṁ,",
  "tha-ap394:59.2_0": "nīlaṁ añjanasannibhaṁ;",
  "tha-ap394:59.3_0": "Sāgaraṁ viya dissantaṁ,",
  "tha-ap394:59.4_0": "passasetaṁ mahāvanaṁ.",
  "tha-ap394:60.1_0": "Ettha so vasate buddho,",
  "tha-ap394:60.2_0": "adantadamako muni;",
  "tha-ap394:60.3_0": "Vinayanto ca veneyye,",
  "tha-ap394:60.4_0": "bodhento bodhipakkhiye’.",
  "tha-ap394:61.1_0": "Pipāsitova udakaṁ,",
  "tha-ap394:61.2_0": "bhojanaṁva jighacchito;",
  "tha-ap394:61.3_0": "Gāvī yathā vacchagiddhā,",
  "tha-ap394:61.4_0": "evāhaṁ viciniṁ jinaṁ.",
  "tha-ap394:62.1_0": "Ācāraupacāraññū,",
  "tha-ap394:62.2_0": "dhammānucchavisaṁvaraṁ;",
  "tha-ap394:62.3_0": "Sikkhāpemi sake sisse,",
  "tha-ap394:62.4_0": "gacchante jinasantikaṁ.",
  "tha-ap394:63.1_0": "‘Durāsadā bhagavanto,",
  "tha-ap394:63.2_0": "sīhāva ekacārino;",
  "tha-ap394:63.3_0": "Pade padaṁ nikkhipantā,",
  "tha-ap394:63.4_0": "āgaccheyyātha māṇavā.",
  "tha-ap394:64.1_0": "Āsīviso yathā ghoro,",
  "tha-ap394:64.2_0": "migarājāva kesarī;",
  "tha-ap394:64.3_0": "Mattova kuñjaro dantī,",
  "tha-ap394:64.4_0": "evaṁ buddhā durāsadā.",
  "tha-ap394:65.1_0": "Ukkāsitañca khipitaṁ,",
  "tha-ap394:65.2_0": "ajjhupekkhiya māṇavā;",
  "tha-ap394:65.3_0": "Pade padaṁ nikkhipantā,",
  "tha-ap394:65.4_0": "upetha buddhasantikaṁ.",
  "tha-ap394:66.1_0": "Paṭisallānagarukā,",
  "tha-ap394:66.2_0": "appasaddā durāsadā;",
  "tha-ap394:66.3_0": "Durūpasaṅkamā buddhā,",
  "tha-ap394:66.4_0": "garū honti sadevake.",
  "tha-ap394:67.1_0": "Yadāhaṁ pañhaṁ pucchāmi,",
  "tha-ap394:67.2_0": "paṭisammodayāmi vā;",
  "tha-ap394:67.3_0": "Appasaddā tadā hotha,",
  "tha-ap394:67.4_0": "munibhūtāva tiṭṭhatha.",
  "tha-ap394:68.1_0": "Yaṁ so deseti sambuddho,",
  "tha-ap394:68.2_0": "khemaṁ nibbānapattiyā;",
  "tha-ap394:68.3_0": "Tamevatthaṁ nisāmetha,",
  "tha-ap394:68.4_0": "saddhammasavanaṁ sukhaṁ’.",
  "tha-ap394:69.1_0": "Upasaṅkamma sambuddhaṁ,",
  "tha-ap394:69.2_0": "sammodiṁ muninā ahaṁ;",
  "tha-ap394:69.3_0": "Taṁ kathaṁ vītisāretvā,",
  "tha-ap394:69.4_0": "lakkhaṇe upadhārayiṁ.",
  "tha-ap394:70.1_0": "Lakkhaṇe dve ca kaṅkhāmi,",
  "tha-ap394:70.2_0": "passāmi tiṁsalakkhaṇe;",
  "tha-ap394:70.3_0": "Kosohitavatthaguyhaṁ,",
  "tha-ap394:70.4_0": "iddhiyā dassayī muni.",
  "tha-ap394:71.1_0": "Jivhaṁ ninnāmayitvāna,",
  "tha-ap394:71.2_0": "kaṇṇasote ca nāsike;",
  "tha-ap394:71.3_0": "Paṭimasi nalāṭantaṁ,",
  "tha-ap394:71.4_0": "kevalaṁ chādayī jino.",
  "tha-ap394:72.1_0": "Tassāhaṁ lakkhaṇe disvā,",
  "tha-ap394:72.2_0": "paripuṇṇe sabyañjane;",
  "tha-ap394:72.3_0": "Buddhoti niṭṭhaṁ gantvāna,",
  "tha-ap394:72.4_0": "saha sissehi pabbajiṁ.",
  "tha-ap394:73.1_0": "Satehi tīhi sahito,",
  "tha-ap394:73.2_0": "pabbajiṁ anagāriyaṁ;",
  "tha-ap394:73.3_0": "Addhamāse asampatte,",
  "tha-ap394:73.4_0": "sabbe pattāmha nibbutiṁ.",
  "tha-ap394:74.1_0": "Ekato kammaṁ katvāna,",
  "tha-ap394:74.2_0": "puññakkhette anuttare;",
  "tha-ap394:74.3_0": "Ekato saṁsaritvāna,",
  "tha-ap394:74.4_0": "ekato vinivattayuṁ.",
  "tha-ap394:75.1_0": "Gopānasiyo datvāna,",
  "tha-ap394:75.2_0": "pūgadhamme vasiṁ ahaṁ;",
  "tha-ap394:75.3_0": "Tena kammena sukatena,",
  "tha-ap394:75.4_0": "aṭṭha hetū labhāmahaṁ.",
  "tha-ap394:76.1_0": "Disāsu pūjito homi,",
  "tha-ap394:76.2_0": "bhogā ca amitā mama;",
  "tha-ap394:76.3_0": "Patiṭṭhā homi sabbesaṁ,",
  "tha-ap394:76.4_0": "tāso mama na vijjati.",
  "tha-ap394:77.1_0": "Byādhayo me na vijjanti,",
  "tha-ap394:77.2_0": "dīghāyuṁ pālayāmi ca;",
  "tha-ap394:77.3_0": "Sukhumacchaviko homi,",
  "tha-ap394:77.4_0": "āvāse patthite vase.",
  "tha-ap394:78.1_0": "Aṭṭha gopānasī datvā,",
  "tha-ap394:78.2_0": "pūgadhamme vasiṁ ahaṁ;",
  "tha-ap394:78.3_0": "Paṭisambhidārahattañca,",
  "tha-ap394:78.4_0": "etaṁ me aparaṭṭhamaṁ.",
  "tha-ap394:79.1_0": "Sabbavositavosāno,",
  "tha-ap394:79.2_0": "katakicco anāsavo;",
  "tha-ap394:79.3_0": "Aṭṭhagopānasī nāma,",
  "tha-ap394:79.4_0": "tava putto mahāmuni.",
  "tha-ap394:80.1_0": "Pañca thambhāni datvāna,",
  "tha-ap394:80.2_0": "pūgadhamme vasiṁ ahaṁ;",
  "tha-ap394:80.3_0": "Tena kammena sukatena,",
  "tha-ap394:80.4_0": "pañca hetū labhāmahaṁ.",
  "tha-ap394:81.1_0": "Acalo homi mettāya,",
  "tha-ap394:81.2_0": "anūnaṅgo bhavāmahaṁ;",
  "tha-ap394:81.3_0": "Ādeyyavacano homi,",
  "tha-ap394:81.4_0": "na dhaṁsemi yathā ahaṁ.",
  "tha-ap394:82.1_0": "Abhantaṁ hoti me cittaṁ,",
  "tha-ap394:82.2_0": "akhilo homi kassaci;",
  "tha-ap394:82.3_0": "Tena kammena sukatena,",
  "tha-ap394:82.4_0": "vimalo homi sāsane.",
  "tha-ap394:83.1_0": "Sagāravo sappatisso,",
  "tha-ap394:83.2_0": "katakicco anāsavo;",
  "tha-ap394:83.3_0": "Sāvako te mahāvīra,",
  "tha-ap394:83.4_0": "bhikkhu taṁ vandate muni.",
  "tha-ap394:84.1_0": "Katvā sukatapallaṅkaṁ,",
  "tha-ap394:84.2_0": "sālāyaṁ paññapesahaṁ;",
  "tha-ap394:84.3_0": "Tena kammena sukatena,",
  "tha-ap394:84.4_0": "pañca hetū labhāmahaṁ.",
  "tha-ap394:85.1_0": "Ucce kule pajāyitvā,",
  "tha-ap394:85.2_0": "mahābhogo bhavāmahaṁ;",
  "tha-ap394:85.3_0": "Sabbasampattiko homi,",
  "tha-ap394:85.4_0": "maccheraṁ me na vijjati.",
  "tha-ap394:86.1_0": "Gamane patthite mayhaṁ,",
  "tha-ap394:86.2_0": "pallaṅko upatiṭṭhati;",
  "tha-ap394:86.3_0": "Saha pallaṅkaseṭṭhena,",
  "tha-ap394:86.4_0": "gacchāmi mama patthitaṁ.",
  "tha-ap394:87.1_0": "Tena pallaṅkadānena,",
  "tha-ap394:87.2_0": "tamaṁ sabbaṁ vinodayiṁ;",
  "tha-ap394:87.3_0": "Sabbābhiññābalappatto,",
  "tha-ap394:87.4_0": "thero vandati taṁ muni.",
  "tha-ap394:88.1_0": "Parakiccattakiccāni,",
  "tha-ap394:88.2_0": "sabbakiccāni sādhayiṁ;",
  "tha-ap394:88.3_0": "Tena kammena sukatena,",
  "tha-ap394:88.4_0": "pāvisiṁ abhayaṁ puraṁ.",
  "tha-ap394:89.1_0": "Pariniṭṭhitasālamhi,",
  "tha-ap394:89.2_0": "paribhogamadāsahaṁ;",
  "tha-ap394:89.3_0": "Tena kammena sukatena,",
  "tha-ap394:89.4_0": "seṭṭhattaṁ ajjhupāgato.",
  "tha-ap394:90.1_0": "Ye keci damakā loke,",
  "tha-ap394:90.2_0": "hatthiasse damenti ye;",
  "tha-ap394:90.3_0": "Karitvā kāraṇā nānā,",
  "tha-ap394:90.4_0": "dāruṇena damenti te.",
  "tha-ap394:91.1_0": "Na hevaṁ tvaṁ mahāvīra,",
  "tha-ap394:91.2_0": "damesi naranāriyo;",
  "tha-ap394:91.3_0": "Adaṇḍena asatthena,",
  "tha-ap394:91.4_0": "damesi uttame dame.",
  "tha-ap394:92.1_0": "Dānassa vaṇṇe kittento,",
  "tha-ap394:92.2_0": "desanākusalo muni;",
  "tha-ap394:92.3_0": "Ekapañhaṁ kathentova,",
  "tha-ap394:92.4_0": "bodhesi tisate muni.",
  "tha-ap394:93.1_0": "Dantā mayaṁ sārathinā,",
  "tha-ap394:93.2_0": "suvimuttā anāsavā;",
  "tha-ap394:93.3_0": "Sabbābhiññābalapattā,",
  "tha-ap394:93.4_0": "nibbutā upadhikkhaye.",
  "tha-ap394:94.1_0": "Satasahassito kappe,",
  "tha-ap394:94.2_0": "yaṁ dānamadadiṁ tadā;",
  "tha-ap394:94.3_0": "Atikkantā bhayā sabbe,",
  "tha-ap394:94.4_0": "sālādānassidaṁ phalaṁ.",
  "tha-ap394:95.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap394:95.2_0": "…pe…",
  "tha-ap394:95.3_0": "viharāmi anāsavo.",
  "tha-ap394:96.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap394:96.2_0": "…pe…",
  "tha-ap394:96.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap394:97.1_0": "Paṭisambhidā catasso,",
  "tha-ap394:97.2_0": "…pe…",
  "tha-ap394:97.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap394:98.1_0": "Itthaṁ sudaṁ āyasmā selo sapariso bhagavato santike imā gāthāyo abhāsitthāti.",
  "tha-ap394:99.1_0": "Selattherassāpadānaṁ dutiyaṁ."
}