{
  "kv13.7:0.1_0": "Kathāvatthu",
  "kv13.7:0.2_0": "Mahāpaṇṇāsaka",
  "kv13.7:0.3_0": "Terasamavagga",
  "kv13.7:0.4_0": "Samāpannoassādetikathā",
  "kv13.7:1.1_0": "Samāpanno assādeti, jhānanikanti jhānārammaṇāti?",
  "kv13.7:1.2_0": "Āmantā.",
  "kv13.7:1.3_0": "Taṁ jhānaṁ tassa jhānassa ārammaṇanti?",
  "kv13.7:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv13.7:1.5_0": "taṁ jhānaṁ tassa jhānassa ārammaṇanti?",
  "kv13.7:1.6_0": "Āmantā.",
  "kv13.7:1.7_0": "Tena phassena taṁ phassaṁ phusati,",
  "kv13.7:1.7_1": "tāya vedanāya taṁ vedanaṁ vedeti,",
  "kv13.7:1.7_2": "tāya saññāya taṁ saññaṁ sañjānāti,",
  "kv13.7:1.7_3": "tāya cetanāya taṁ cetanaṁ ceteti,",
  "kv13.7:1.7_4": "tena cittena taṁ cittaṁ cinteti,",
  "kv13.7:1.7_5": "tena vitakkena taṁ vitakkaṁ vitakketi,",
  "kv13.7:1.7_6": "tena vicārena taṁ vicāraṁ vicāreti,",
  "kv13.7:1.7_7": "tāya pītiyā taṁ pītiṁ piyāyati,",
  "kv13.7:1.7_8": "tāya satiyā taṁ satiṁ sarati,",
  "kv13.7:1.7_9": "tāya paññāya taṁ paññaṁ pajānātīti?",
  "kv13.7:1.8_0": "Na hevaṁ vattabbe …pe….",
  "kv13.7:2.1_0": "Jhānanikanti cittasampayuttā, jhānaṁ cittasampayuttanti?",
  "kv13.7:2.2_0": "Āmantā.",
  "kv13.7:2.3_0": "Dvinnaṁ phassānaṁ …pe…",
  "kv13.7:2.4_0": "dvinnaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv13.7:2.5_0": "Na hevaṁ vattabbe …pe….",
  "kv13.7:3.1_0": "Jhānanikanti akusalaṁ, jhānaṁ kusalanti?",
  "kv13.7:3.2_0": "Āmantā.",
  "kv13.7:3.3_0": "Kusalākusalā sāvajjānavajjā hīnapaṇītā kaṇhasukkasappaṭibhāgā dhammā sammukhībhāvaṁ āgacchantīti?",
  "kv13.7:3.4_0": "Na hevaṁ vattabbe …pe….",
  "kv13.7:4.1_0": "Kusalākusalā sāvajjānavajjā hīnapaṇītā kaṇhasukkasappaṭibhāgā dhammā sammukhībhāvaṁ āgacchantīti?",
  "kv13.7:4.2_0": "Āmantā.",
  "kv13.7:4.3_0": "Nanu vuttaṁ bhagavatā—",
  "kv13.7:4.4_0": "“cattārimāni, bhikkhave, suvidūravidūrāni.",
  "kv13.7:4.5_0": "Katamāni cattāri?",
  "kv13.7:4.6_0": "Nabhañca, bhikkhave, pathavī ca—",
  "kv13.7:4.7_0": "idaṁ paṭhamaṁ suvidūravidūraṁ …pe… tasmā sataṁ dhammo asabbhi ārakā”ti.",
  "kv13.7:4.8_0": "Attheva suttantoti?",
  "kv13.7:4.9_0": "Āmantā.",
  "kv13.7:4.10_0": "Tena hi na vattabbaṁ—",
  "kv13.7:4.11_0": "“kusalākusalā sāvajjānavajjā hīnapaṇītā kaṇhasukkasappaṭibhāgā dhammā sammukhībhāvaṁ āgacchantī”ti.",
  "kv13.7:5.1_0": "Na vattabbaṁ—",
  "kv13.7:5.2_0": "“samāpanno assādeti, jhānanikanti jhānārammaṇā”ti?",
  "kv13.7:5.3_0": "Āmantā.",
  "kv13.7:5.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv13.7:5.5_0": "“idha, bhikkhave,",
  "kv13.7:5.5_1": "bhikkhu vivicceva kāmehi vivicca akusalehi dhammehi paṭhamaṁ jhānaṁ upasampajja viharati,",
  "kv13.7:5.5_2": "so taṁ assādeti taṁ nikāmeti tena ca vittiṁ āpajjati;",
  "kv13.7:5.6_0": "vitakkavicārānaṁ vūpasamā …pe… dutiyaṁ jhānaṁ …pe…",
  "kv13.7:5.7_0": "tatiyaṁ jhānaṁ …pe…",
  "kv13.7:5.8_0": "catutthaṁ jhānaṁ upasampajja viharati,",
  "kv13.7:5.8_1": "so taṁ assādeti taṁ nikāmeti tena ca vittiṁ āpajjatī”ti.",
  "kv13.7:5.9_0": "Attheva suttantoti?",
  "kv13.7:5.10_0": "Āmantā.",
  "kv13.7:5.11_0": "Tena hi samāpanno assādeti, jhānanikanti jhānārammaṇāti.",
  "kv13.7:5.12_0": "Samāpanno assādetikathā niṭṭhitā."
}