{
  "tha-ap355:0.1_0": "Therāpadāna",
  "tha-ap355:0.2_0": "Saddasaññakavagga",
  "tha-ap355:0.3_0": "3. Kiṁsukapūjakattheraapadāna",
  "tha-ap355:1.1_0": "“Kiṁsukaṁ pupphitaṁ disvā,",
  "tha-ap355:1.2_0": "paggahetvāna añjaliṁ;",
  "tha-ap355:1.3_0": "Buddhaṁ saritvā siddhatthaṁ,",
  "tha-ap355:1.4_0": "ākāse abhipūjayiṁ.",
  "tha-ap355:2.1_0": "Catunnavutito kappe,",
  "tha-ap355:2.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap355:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap355:2.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap355:3.1_0": "Paṭisambhidā catasso,",
  "tha-ap355:3.2_0": "…pe…",
  "tha-ap355:3.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap355:4.1_0": "Itthaṁ sudaṁ āyasmā kiṁsukapūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap355:5.1_0": "Kiṁsukapūjakattherassāpadānaṁ tatiyaṁ."
}