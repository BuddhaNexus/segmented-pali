{
  "kv16.2:0.1_0": "Kathāvatthu",
  "kv16.2:0.2_0": "Niggahapaṇṇāsaka",
  "kv16.2:0.3_0": "Soḷasamavagga",
  "kv16.2:0.4_0": "Paggahakathā",
  "kv16.2:1.1_0": "Paro parassa cittaṁ paggaṇhātīti?",
  "kv16.2:1.2_0": "Āmantā.",
  "kv16.2:1.3_0": "Paro parassa cittaṁ “mā rajji”, “mā dussi”, “mā muyhi”, “mā kilissī”ti paggaṇhātīti?",
  "kv16.2:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv16.2:1.5_0": "paro parassa cittaṁ paggaṇhātīti?",
  "kv16.2:1.6_0": "Āmantā.",
  "kv16.2:1.7_0": "Paro parassa alobhaṁ kusalamūlaṁ janeti …",
  "kv16.2:1.8_0": "adosaṁ kusalamūlaṁ janeti …",
  "kv16.2:1.9_0": "amohaṁ kusalamūlaṁ janeti …",
  "kv16.2:1.10_0": "saddhaṁ janeti …",
  "kv16.2:1.11_0": "vīriyaṁ janeti …",
  "kv16.2:1.12_0": "satiṁ janeti …",
  "kv16.2:1.13_0": "samādhiṁ janeti …",
  "kv16.2:1.14_0": "paññaṁ janetīti?",
  "kv16.2:1.15_0": "Na hevaṁ vattabbe …pe…",
  "kv16.2:1.16_0": "paro parassa cittaṁ paggaṇhātīti?",
  "kv16.2:1.17_0": "Āmantā.",
  "kv16.2:1.18_0": "Paro parassa uppanno phasso “mā nirujjhī”ti paggaṇhātīti?",
  "kv16.2:1.19_0": "Na hevaṁ vattabbe …pe…",
  "kv16.2:1.20_0": "paro parassa uppannā vedanā …pe…",
  "kv16.2:1.21_0": "uppannā paññā “mā nirujjhī”ti paggaṇhātīti?",
  "kv16.2:1.22_0": "Na hevaṁ vattabbe …pe….",
  "kv16.2:2.1_0": "Paro parassa cittaṁ paggaṇhātīti?",
  "kv16.2:2.2_0": "Āmantā.",
  "kv16.2:2.3_0": "Paro parassa atthāya rāgaṁ pajahati …",
  "kv16.2:2.4_0": "dosaṁ pajahati …",
  "kv16.2:2.5_0": "mohaṁ pajahati …pe…",
  "kv16.2:2.6_0": "anottappaṁ pajahatīti?",
  "kv16.2:2.7_0": "Na hevaṁ vattabbe …pe….",
  "kv16.2:3.1_0": "Paro parassa cittaṁ paggaṇhātīti?",
  "kv16.2:3.2_0": "Āmantā.",
  "kv16.2:3.3_0": "Paro parassa atthāya maggaṁ bhāveti …",
  "kv16.2:3.4_0": "satipaṭṭhānaṁ bhāveti …pe…",
  "kv16.2:3.5_0": "bojjhaṅgaṁ bhāvetīti?",
  "kv16.2:3.6_0": "Na hevaṁ vattabbe …pe….",
  "kv16.2:4.1_0": "Paro parassa cittaṁ paggaṇhātīti?",
  "kv16.2:4.2_0": "Āmantā.",
  "kv16.2:4.3_0": "Paro parassa atthāya dukkhaṁ parijānāti …pe…",
  "kv16.2:4.4_0": "maggaṁ bhāvetīti?",
  "kv16.2:4.5_0": "Na hevaṁ vattabbe …pe….",
  "kv16.2:5.1_0": "Paro parassa cittaṁ paggaṇhātīti?",
  "kv16.2:5.2_0": "Āmantā.",
  "kv16.2:5.3_0": "Añño aññassa kārako,",
  "kv16.2:5.3_1": "paraṅkataṁ sukhaṁ dukkhaṁ añño karoti añño paṭisaṁvedetīti?",
  "kv16.2:5.4_0": "Na hevaṁ vattabbe …pe….",
  "kv16.2:6.1_0": "Paro parassa cittaṁ paggaṇhātīti?",
  "kv16.2:6.2_0": "Āmantā.",
  "kv16.2:6.3_0": "Nanu vuttaṁ bhagavatā—",
  "kv16.2:7.1_0": "“Attanāva kataṁ pāpaṁ …pe…",
  "kv16.2:7.2_0": "Nāñño aññaṁ visodhaye”ti.",
  "kv16.2:8.1_0": "Attheva suttantoti?",
  "kv16.2:8.2_0": "Āmantā.",
  "kv16.2:8.3_0": "Tena hi na vattabbaṁ—",
  "kv16.2:8.4_0": "“paro parassa cittaṁ paggaṇhātī”ti.",
  "kv16.2:9.1_0": "Na vattabbaṁ—",
  "kv16.2:9.2_0": "“paro parassa cittaṁ paggaṇhātī”ti?",
  "kv16.2:9.3_0": "Āmantā.",
  "kv16.2:9.4_0": "Nanu atthi balappattā, atthi vasībhūtāti?",
  "kv16.2:9.5_0": "Āmantā.",
  "kv16.2:9.6_0": "Hañci atthi balappattā, atthi vasībhūtā, tena vata re vattabbe—",
  "kv16.2:9.7_0": "“paro parassa cittaṁ paggaṇhātī”ti.",
  "kv16.2:9.8_0": "Paggahakathā niṭṭhitā."
}