{
  "kv1.2:0.1_0": "Kathāvatthu",
  "kv1.2:0.2_0": "Mahāpaṇṇāsaka",
  "kv1.2:0.3_0": "Paṭhamavagga",
  "kv1.2:0.4_0": "Parihānikathā",
  "kv1.2:1.0_0": "Vādayuttiparihāni",
  "kv1.2:1.1_0": "Parihāyati arahā arahattāti?",
  "kv1.2:1.2_0": "Āmantā.",
  "kv1.2:1.3_0": "Sabbattha arahā arahattā parihāyatīti?",
  "kv1.2:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv1.2:1.5_0": "sabbattha arahā arahattā parihāyatīti?",
