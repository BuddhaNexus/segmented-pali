{
  "kv17.1:0.1_0": "Kathāvatthu",
  "kv17.1:0.2_0": "Niggahapaṇṇāsaka",
  "kv17.1:0.3_0": "Sattarasamavagga",
  "kv17.1:0.4_0": "Atthiarahatopuññūpacayotikathā",
  "kv17.1:1.1_0": "Atthi arahato puññūpacayoti?",
  "kv17.1:1.2_0": "Āmantā.",
  "kv17.1:1.3_0": "Atthi arahato apuññūpacayoti?",
  "kv17.1:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv17.1:1.5_0": "natthi arahato apuññūpacayoti?",
  "kv17.1:1.6_0": "Āmantā.",
  "kv17.1:1.7_0": "Natthi arahato puññūpacayoti?",
  "kv17.1:1.8_0": "Na hevaṁ vattabbe …pe….",
  "kv17.1:2.1_0": "Atthi arahato puññūpacayoti?",
  "kv17.1:2.2_0": "Āmantā.",
  "kv17.1:2.3_0": "Arahā puññābhisaṅkhāraṁ abhisaṅkharoti,",
  "kv17.1:2.3_1": "āneñjābhisaṅkhāraṁ abhisaṅkharoti,",
  "kv17.1:2.3_2": "gatisaṁvattaniyaṁ kammaṁ karoti,",
  "kv17.1:2.3_3": "bhavasaṁvattaniyaṁ kammaṁ karoti,",
  "kv17.1:2.3_4": "issariyasaṁvattaniyaṁ kammaṁ karoti,",
  "kv17.1:2.3_5": "adhipaccasaṁvattaniyaṁ kammaṁ karoti,",
  "kv17.1:2.3_6": "mahābhogasaṁvattaniyaṁ kammaṁ karoti,",
  "kv17.1:2.3_7": "mahāparivārasaṁvattaniyaṁ kammaṁ karoti,",
  "kv17.1:2.3_8": "devasobhagyasaṁvattaniyaṁ kammaṁ karoti,",
  "kv17.1:2.3_9": "manussasobhagyasaṁvattaniyaṁ kammaṁ karotīti?",
  "kv17.1:2.4_0": "Na hevaṁ vattabbe …pe….",
  "kv17.1:3.1_0": "Atthi arahato puññūpacayoti?",
  "kv17.1:3.2_0": "Āmantā.",
  "kv17.1:3.3_0": "Arahā ācinātīti?",
  "kv17.1:3.4_0": "Na hevaṁ vattabbe …pe…",
  "kv17.1:3.5_0": "arahā apacinātīti?",
  "kv17.1:3.6_0": "Na hevaṁ vattabbe …pe…",
  "kv17.1:3.7_0": "arahā pajahatīti …pe…",
  "kv17.1:3.8_0": "arahā upādiyatīti …pe…",
  "kv17.1:3.9_0": "arahā visinetīti …pe…",
  "kv17.1:3.10_0": "arahā ussinetīti …pe…",
  "kv17.1:3.11_0": "arahā vidhūpetīti …pe…",
  "kv17.1:3.12_0": "arahā sandhūpetīti?",
  "kv17.1:3.13_0": "Na hevaṁ vattabbe …pe…",
  "kv17.1:3.14_0": "nanu arahā nevācināti na apacināti apacinitvā ṭhitoti?",
  "kv17.1:3.15_0": "Āmantā.",
  "kv17.1:3.16_0": "Hañci arahā nevācināti nāpacināti apacinitvā ṭhito, no ca vata re vattabbe—",
  "kv17.1:3.17_0": "“atthi arahato puññūpacayo”ti.",
  "kv17.1:4.1_0": "Nanu arahā neva pajahati na upādiyati pajahitvā ṭhito,",
  "kv17.1:4.1_1": "neva visineti na ussineti visinetvā ṭhito,",
  "kv17.1:4.1_2": "neva vidhūpeti na sandhūpeti vidhūpetvā ṭhitoti?",
  "kv17.1:4.2_0": "Āmantā.",
  "kv17.1:4.3_0": "Hañci arahā neva vidhūpeti na sandhūpeti vidhūpetvā ṭhito, no ca vata re vattabbe—",
  "kv17.1:4.4_0": "“atthi arahato puññūpacayo”ti.",
  "kv17.1:5.1_0": "Natthi arahato puññūpacayoti?",
  "kv17.1:5.2_0": "Āmantā.",
  "kv17.1:5.3_0": "Arahā dānaṁ dadeyyāti?",
  "kv17.1:5.4_0": "Āmantā.",
  "kv17.1:5.5_0": "Hañci arahā dānaṁ dadeyya, no ca vata re vattabbe—",
  "kv17.1:5.6_0": "“natthi arahato puññūpacayo”ti.",
  "kv17.1:6.1_0": "Arahā cīvaraṁ dadeyya …pe…",
  "kv17.1:6.2_0": "piṇḍapātaṁ dadeyya …",
  "kv17.1:6.3_0": "senāsanaṁ dadeyya …",
  "kv17.1:6.4_0": "gilānapaccayabhesajjaparikkhāraṁ dadeyya …",
  "kv17.1:6.5_0": "khādanīyaṁ dadeyya …",
  "kv17.1:6.6_0": "bhojanīyaṁ dadeyya …",
  "kv17.1:6.7_0": "pānīyaṁ dadeyya …",
  "kv17.1:6.8_0": "cetiyaṁ vandeyya …",
  "kv17.1:6.9_0": "cetiye mālaṁ āropeyya …",
  "kv17.1:6.10_0": "gandhaṁ āropeyya …",
  "kv17.1:6.11_0": "vilepanaṁ āropeyya …pe…",
  "kv17.1:6.12_0": "cetiyaṁ abhidakkhiṇaṁ kareyyāti?",
  "kv17.1:6.13_0": "Āmantā.",
  "kv17.1:6.14_0": "Hañci arahā cetiyaṁ abhidakkhiṇaṁ kareyya, no ca vata re vattabbe—",
  "kv17.1:6.15_0": "“natthi arahato puññūpacayo”ti.",
  "kv17.1:6.16_0": "Atthi arahato puññūpacayotikathā niṭṭhitā."
}