{
  "ne24:0.1_0": "Netti",
  "ne24:0.2_0": "Paṭiniddesavāra",
  "ne24:0.3_0": "Sampāta 5",
  "ne24:0.4_0": "Lakkhaṇahārasampāta",
  "ne24:1.1_0": "Tattha katamo lakkhaṇo hārasampāto?",
  "ne24:2.1_0": "“Tasmā rakkhitacittassa, sammāsaṅkappagocaro”ti gāthā.",
  "ne24:2.2_0": "“Tasmā rakkhitacittassa,",
  "ne24:2.2_1": "sammāsaṅkappagocaro”ti idaṁ satindriyaṁ,",
  "ne24:2.2_2": "satindriye gahite gahitāni bhavanti pañcindriyāni.",
  "ne24:2.3_0": "“Sammādiṭṭhipurekkhāro”ti sammādiṭṭhiyā gahitāya gahito bhavati ariyo aṭṭhaṅgiko maggo.",
  "ne24:2.4_0": "Taṁ kissa hetu?",
  "ne24:2.5_0": "Sammādiṭṭhito hi sammāsaṅkappo pabhavati,",
  "ne24:2.5_1": "sammāsaṅkappato sammāvācā pabhavati,",
  "ne24:2.5_2": "sammāvācāto sammākammanto pabhavati,",
  "ne24:2.5_3": "sammākammantato sammāājīvo pabhavati,",
  "ne24:2.5_4": "sammāājīvato sammāvāyāmo pabhavati,",
  "ne24:2.5_5": "sammāvāyāmato sammāsati pabhavati,",
  "ne24:2.5_6": "sammāsatito sammāsamādhi pabhavati,",
  "ne24:2.5_7": "sammāsamādhito sammāvimutti pabhavati,",
  "ne24:2.5_8": "sammāvimuttito sammāvimuttiñāṇadassanaṁ pabhavati.",
  "ne24:2.6_0": "Niyutto lakkhaṇo hārasampāto."
}