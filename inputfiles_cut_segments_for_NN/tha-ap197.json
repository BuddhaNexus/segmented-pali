{
  "tha-ap197:0.1_0": "Therāpadāna",
  "tha-ap197:0.2_0": "Tamālapupphiyavagga",
  "tha-ap197:0.3_0": "5. Aṅkolakattheraapadāna",
  "tha-ap197:1.1_0": "“Aṅkolaṁ pupphitaṁ disvā,",
  "tha-ap197:1.2_0": "mālāvaraṁ sakosakaṁ;",
  "tha-ap197:1.3_0": "Ocinitvāna taṁ pupphaṁ,",
  "tha-ap197:1.4_0": "agamaṁ buddhasantikaṁ.",
  "tha-ap197:2.1_0": "Siddhattho tamhi samaye,",
  "tha-ap197:2.2_0": "patilīno mahāmuni;",
  "tha-ap197:2.3_0": "Muhuttaṁ paṭimānetvā,",
  "tha-ap197:2.4_0": "guhāyaṁ pupphamokiriṁ.",
  "tha-ap197:3.1_0": "Catunnavutito kappe,",
  "tha-ap197:3.2_0": "Yaṁ pupphamabhipūjayiṁ;",
  "tha-ap197:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap197:3.4_0": "Pupphadānassidaṁ phalaṁ.",
  "tha-ap197:4.1_0": "Chattiṁsamhi ito kappe,",
  "tha-ap197:4.2_0": "āseko devagajjito;",
  "tha-ap197:4.3_0": "Sattaratanasampanno,",
  "tha-ap197:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap197:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap197:5.2_0": "…pe…",
  "tha-ap197:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap197:6.1_0": "Itthaṁ sudaṁ āyasmā aṅkolako thero imā gāthāyo abhāsitthāti.",
  "tha-ap197:7.1_0": "Aṅkolakattherassāpadānaṁ pañcamaṁ."
}