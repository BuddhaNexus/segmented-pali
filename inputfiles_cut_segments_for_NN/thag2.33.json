{
  "thag2.33:0.1_0": "Theragāthā",
  "thag2.33:0.2_0": "Dukanipāta",
  "thag2.33:0.3_0": "Catutthavagga",
  "thag2.33:0.4_0": "2.33 Upavāṇattheragāthā",
  "thag2.33:1.1_0": "“Arahaṁ sugato loke,",
  "thag2.33:1.2_0": "vātehābādhiko muni;",
  "thag2.33:1.3_0": "Sace uṇhodakaṁ atthi,",
  "thag2.33:1.4_0": "munino dehi brāhmaṇa.",
  "thag2.33:2.1_0": "Pūjito pūjaneyyānaṁ,",
  "thag2.33:2.2_0": "sakkareyyāna sakkato;",
  "thag2.33:2.3_0": "Apacitopaceyyānaṁ,",
  "thag2.33:2.4_0": "tassa icchāmi hātave”ti.",
  "thag2.33:2.5_0": "… Upavāṇo thero …."
}