{
  "mil3.7.3:0.1_0": "Milindapañha",
  "mil3.7.3:0.2_0": "Arūpadhammavavatthānavagga",
  "mil3.7.3:0.3_0": "3. Dukkhappahānavāyamapañha",
  "mil3.7.3:1.1_0": "Rājā āha—",
  "mil3.7.3:1.2_0": "“bhante nāgasena,",
  "mil3.7.3:1.2_1": "kiṁ tumhe atītassa dukkhassa pahānāya vāyamathā”ti?",
  "mil3.7.3:1.3_0": "“Na hi, mahārājā”ti.",
  "mil3.7.3:1.4_0": "“Kiṁ pana, bhante,",
  "mil3.7.3:1.4_1": "anāgatassa dukkhassa pahānāya vāyamathā”ti?",
  "mil3.7.3:1.5_0": "“Na hi, mahārājā”ti.",
  "mil3.7.3:1.6_0": "“Kiṁ pana paccuppannassa dukkhassa pahānāya vāyamathā”ti?",
  "mil3.7.3:1.7_0": "“Na hi, mahārājā”ti.",
  "mil3.7.3:1.8_0": "“Yadi tumhe na atītassa dukkhassa pahānāya vāyamatha,",
  "mil3.7.3:1.8_1": "na anāgatassa dukkhassa pahānāya vāyamatha,",
  "mil3.7.3:1.8_2": "na paccuppannassa dukkhassa pahānāya vāyamatha, atha kimatthāya vāyamathā”ti.",
  "mil3.7.3:1.9_0": "Thero āha—",
  "mil3.7.3:1.10_0": "“‘kinti, mahārāja, idañca dukkhaṁ nirujjheyya,",
  "mil3.7.3:1.10_1": "aññañca dukkhaṁ nuppajjeyyā’ti etadatthāya vāyamāmā”ti.",
  "mil3.7.3:2.1_0": "“Atthi pana te, bhante nāgasena, anāgataṁ dukkhan”ti?",
  "mil3.7.3:2.2_0": "“Natthi, mahārājā”ti.",
  "mil3.7.3:2.3_0": "“Tumhe kho, bhante nāgasena, atipaṇḍitā,",
  "mil3.7.3:2.3_1": "ye tumhe asantānaṁ anāgatānaṁ dukkhānaṁ pahānāya vāyamathā”ti?",
  "mil3.7.3:2.4_0": "“Atthi pana te, mahārāja,",
  "mil3.7.3:2.4_1": "keci paṭirājāno paccatthikā paccāmittā paccupaṭṭhitā hontī”ti?",
  "mil3.7.3:2.5_0": "“Āma, bhante, atthī”ti.",
  "mil3.7.3:2.6_0": "“Kiṁ nu kho, mahārāja,",
  "mil3.7.3:2.6_1": "tadā tumhe parikhaṁ khaṇāpeyyātha,",
  "mil3.7.3:2.6_2": "pākāraṁ cināpeyyātha gopuraṁ kārāpeyyātha, aṭṭālakaṁ kārāpeyyātha, dhaññaṁ atiharāpeyyāthā”ti?",
  "mil3.7.3:2.7_0": "“Na hi, bhante,",
  "mil3.7.3:2.7_1": "paṭikacceva taṁ paṭiyattaṁ hotī”ti.",
  "mil3.7.3:2.8_0": "“Kiṁ tumhe, mahārāja,",
  "mil3.7.3:2.8_1": "tadā hatthismiṁ sikkheyyātha, assasmiṁ sikkheyyātha, rathasmiṁ sikkheyyātha, dhanusmiṁ sikkheyyātha, tharusmiṁ sikkheyyāthā”ti?",
  "mil3.7.3:2.9_0": "“Na hi, bhante, paṭikacceva taṁ sikkhitaṁ hotī”ti.",
  "mil3.7.3:2.10_0": "“Kissatthāyā”ti?",
  "mil3.7.3:2.11_0": "“Anāgatānaṁ, bhante, bhayānaṁ paṭibāhanatthāyā”ti.",
  "mil3.7.3:2.12_0": "“Kiṁ nu kho, mahārāja, atthi anāgataṁ bhayan”ti?",
  "mil3.7.3:2.13_0": "“Natthi, bhante”ti.",
  "mil3.7.3:2.14_0": "“Tumhe ca kho, mahārāja, atipaṇḍitā,",
  "mil3.7.3:2.14_1": "ye tumhe asantānaṁ anāgatānaṁ bhayānaṁ paṭibāhanatthāya paṭiyādethā”ti.",
  "mil3.7.3:3.1_0": "“Bhiyyo opammaṁ karohī”ti.",
  "mil3.7.3:3.2_0": "“Taṁ kiṁ maññasi, mahārāja,",
  "mil3.7.3:3.2_1": "yadā tvaṁ pipāsito bhaveyyāsi,",
  "mil3.7.3:3.2_2": "tadā tvaṁ udapānaṁ khaṇāpeyyāsi, pokkharaṇiṁ khaṇāpeyyāsi,",
  "mil3.7.3:3.2_3": "taḷākaṁ khaṇāpeyyāsi ‘pānīyaṁ pivissāmī’”ti?",
  "mil3.7.3:3.3_0": "“Na hi, bhante,",
  "mil3.7.3:3.3_1": "paṭikacceva taṁ paṭiyattaṁ hotī”ti.",
  "mil3.7.3:3.4_0": "“Kissatthāyā”ti?",
  "mil3.7.3:3.5_0": "“Anāgatānaṁ, bhante,",
  "mil3.7.3:3.5_1": "pipāsānaṁ paṭibāhanatthāya paṭiyattaṁ hotī”ti.",
  "mil3.7.3:3.6_0": "“Atthi pana, mahārāja, anāgatā pipāsā”ti?",
  "mil3.7.3:3.7_0": "“Natthi, bhante”ti.",
  "mil3.7.3:3.8_0": "“Tumhe kho, mahārāja, atipaṇḍitā,",
  "mil3.7.3:3.8_1": "ye tumhe asantānaṁ anāgatānaṁ pipāsānaṁ paṭibāhanatthāya taṁ paṭiyādethā”ti.",
  "mil3.7.3:4.1_0": "“Bhiyyo opammaṁ karohī”ti.",
  "mil3.7.3:4.2_0": "“Taṁ kiṁ maññasi, mahārāja,",
  "mil3.7.3:4.2_1": "yadā tvaṁ bubhukkhito bhaveyyāsi,",
  "mil3.7.3:4.2_2": "tadā tvaṁ khettaṁ kasāpeyyāsi,",
  "mil3.7.3:4.2_3": "sāliṁ vapāpeyyāsi ‘bhattaṁ bhuñjissāmī’”ti?",
  "mil3.7.3:4.3_0": "“Na hi, bhante,",
  "mil3.7.3:4.3_1": "paṭikacceva taṁ paṭiyattaṁ hotī”ti.",
  "mil3.7.3:4.4_0": "“Kissatthāyā”ti.",
  "mil3.7.3:4.5_0": "“Anāgatānaṁ, bhante,",
  "mil3.7.3:4.5_1": "bubhukkhānaṁ paṭibāhanatthāyā”ti.",
  "mil3.7.3:4.6_0": "“Atthi pana, mahārāja, anāgatā bubhukkhā”ti?",
  "mil3.7.3:4.7_0": "“Natthi, bhante”ti.",
  "mil3.7.3:4.8_0": "“Tumhe kho, mahārāja, atipaṇḍitā,",
  "mil3.7.3:4.8_1": "ye tumhe asantānaṁ anāgatānaṁ bubhukkhānaṁ paṭibāhanatthāya paṭiyādethā”ti.",
  "mil3.7.3:5.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.7.3:6.1_0": "Dukkhappahānavāyamapañho tatiyo."
}