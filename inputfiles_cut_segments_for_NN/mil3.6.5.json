{
  "mil3.6.5:0.1_0": "Milindapañha",
  "mil3.6.5:0.2_0": "Sativagga",
  "mil3.6.5:0.3_0": "5. Bhagavatoupasampadāpañha",
  "mil3.6.5:1.1_0": "Rājā āha—",
  "mil3.6.5:1.2_0": "“bhante nāgasena, upasampadā sundarā”ti?",
  "mil3.6.5:1.3_0": "“Āma, mahārāja, upasampadā sundarā”ti.",
  "mil3.6.5:1.4_0": "“Atthi pana, bhante, buddhassa upasampadā, udāhu natthī”ti?",
  "mil3.6.5:1.5_0": "“Upasampanno kho, mahārāja,",
  "mil3.6.5:1.5_1": "bhagavā bodhirukkhamūle saha sabbaññutañāṇena,",
  "mil3.6.5:1.5_2": "natthi bhagavato upasampadā aññehi dinnā,",
  "mil3.6.5:1.5_3": "yathā sāvakānaṁ, mahārāja,",
  "mil3.6.5:1.5_4": "bhagavā sikkhāpadaṁ paññapeti yāvajīvaṁ anatikkamanīyan”ti.",
  "mil3.6.5:2.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.6.5:3.1_0": "Bhagavato upasampadāpañho pañcamo."
}