{
  "thag1.66:0.1_0": "Theragāthā",
  "thag1.66:0.2_0": "Ekakanipāta",
  "thag1.66:0.3_0": "Sattamavagga",
  "thag1.66:0.4_0": "1.66. Meghiyattheragāthā",
  "thag1.66:1.1_0": "“Anusāsi mahāvīro,",
  "thag1.66:1.2_0": "sabbadhammāna pāragū;",
  "thag1.66:1.3_0": "Tassāhaṁ dhammaṁ sutvāna,",
  "thag1.66:1.4_0": "vihāsiṁ santike sato;",
  "thag1.66:1.5_0": "Tisso vijjā anuppattā,",
  "thag1.66:1.6_0": "kataṁ buddhassa sāsanan”ti.",
  "thag1.66:1.7_0": "… Meghiyo thero …."
}