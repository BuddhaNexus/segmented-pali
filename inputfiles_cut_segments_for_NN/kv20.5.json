{
  "kv20.5:0.1_0": "Kathāvatthu",
  "kv20.5:0.2_0": "Mahāpaṇṇāsaka",
  "kv20.5:0.3_0": "Vīsatimavagga",
  "kv20.5:0.4_0": "Maggakathā",
  "kv20.5:1.1_0": "Pañcaṅgiko maggoti?",
  "kv20.5:1.2_0": "Āmantā.",
  "kv20.5:1.3_0": "Nanu aṭṭhaṅgiko maggo vutto bhagavatā, seyyathidaṁ—",
  "kv20.5:1.4_0": "sammādiṭṭhi …pe… sammāsamādhīti?",
  "kv20.5:1.5_0": "Āmantā.",
  "kv20.5:1.6_0": "Hañci aṭṭhaṅgiko maggo vutto bhagavatā, seyyathidaṁ—",
  "kv20.5:1.7_0": "sammādiṭṭhi …pe… sammāsamādhi, no ca vata re vattabbe—",
  "kv20.5:1.8_0": "“pañcaṅgiko maggo”ti.",
  "kv20.5:2.1_0": "Pañcaṅgiko maggoti?",
  "kv20.5:2.2_0": "Āmantā.",
  "kv20.5:2.3_0": "Nanu vuttaṁ bhagavatā—",
  "kv20.5:3.1_0": "“Maggānaṁ aṭṭhaṅgiko seṭṭho,",
  "kv20.5:3.2_0": "Saccānaṁ caturo padā;",
  "kv20.5:3.3_0": "Virāgo seṭṭho dhammānaṁ,",
  "kv20.5:3.4_0": "Dvipadānañca cakkhumā”ti.",
  "kv20.5:4.1_0": "Attheva suttantoti?",
  "kv20.5:4.2_0": "Āmantā.",
  "kv20.5:4.3_0": "Tena hi aṭṭhaṅgiko maggoti.",
  "kv20.5:5.1_0": "Sammāvācā maggaṅgaṁ, sā ca na maggoti?",
  "kv20.5:5.2_0": "Āmantā.",
  "kv20.5:5.3_0": "Sammādiṭṭhi maggaṅgaṁ, sā ca na maggoti?",
  "kv20.5:5.4_0": "Na hevaṁ vattabbe …pe…",
  "kv20.5:5.5_0": "sammāvācā maggaṅgaṁ, sā ca na maggoti?",
  "kv20.5:5.6_0": "Āmantā.",
  "kv20.5:5.7_0": "Sammāsaṅkappo …pe…",
  "kv20.5:5.8_0": "sammāvāyāmo …pe…",
  "kv20.5:5.9_0": "sammāsati …pe…",
  "kv20.5:5.10_0": "sammāsamādhi maggaṅgaṁ, so ca na maggoti?",
  "kv20.5:5.11_0": "Na hevaṁ vattabbe …pe….",
  "kv20.5:6.1_0": "Sammākammanto …pe…",
  "kv20.5:6.2_0": "sammāājīvo maggaṅgaṁ, so ca na maggoti?",
  "kv20.5:6.3_0": "Āmantā.",
  "kv20.5:6.4_0": "sammādiṭṭhi …pe… sammāsamādhi maggaṅgaṁ, so ca na maggoti?",
  "kv20.5:6.5_0": "Na hevaṁ vattabbe …pe….",
  "kv20.5:7.1_0": "Sammādiṭṭhi maggaṅgaṁ, sā ca maggoti?",
  "kv20.5:7.2_0": "Āmantā.",
  "kv20.5:7.3_0": "Sammāvācā maggaṅgaṁ, sā ca maggoti?",
  "kv20.5:7.4_0": "Na hevaṁ vattabbe …pe…",
  "kv20.5:7.5_0": "sammādiṭṭhi maggaṅgaṁ, sā ca maggoti?",
  "kv20.5:7.6_0": "Āmantā.",
  "kv20.5:7.7_0": "Sammākammanto …pe…",
  "kv20.5:7.8_0": "sammāājīvo maggaṅgaṁ, so ca maggoti?",
  "kv20.5:7.9_0": "Na hevaṁ vattabbe …pe….",
  "kv20.5:8.1_0": "Sammāsaṅkappo …pe…",
  "kv20.5:8.2_0": "sammāvāyāmo …pe…",
  "kv20.5:8.3_0": "sammāsati …pe…",
  "kv20.5:8.4_0": "sammāsamādhi maggaṅgaṁ, so ca maggoti?",
  "kv20.5:8.5_0": "Āmantā.",
  "kv20.5:8.6_0": "Sammāvācā …pe…",
  "kv20.5:8.7_0": "sammākammanto …pe…",
  "kv20.5:8.8_0": "sammāājīvo maggaṅgaṁ, so ca maggoti?",
  "kv20.5:8.9_0": "Na hevaṁ vattabbe …pe….",
  "kv20.5:9.1_0": "Ariyo aṭṭhaṅgiko maggoti?",
  "kv20.5:9.2_0": "Āmantā.",
  "kv20.5:9.3_0": "Nanu vuttaṁ bhagavatā—",
  "kv20.5:9.4_0": "“pubbeva kho panassa kāyakammaṁ vacīkammaṁ ājīvo suparisuddho hoti;",
  "kv20.5:9.5_0": "evamassāyaṁ ariyo aṭṭhaṅgiko maggo bhāvanāpāripūriṁ gacchatī”ti.",
  "kv20.5:9.6_0": "Attheva suttantoti?",
  "kv20.5:9.7_0": "Āmantā.",
  "kv20.5:9.8_0": "Tena hi pañcaṅgiko maggoti.",
  "kv20.5:10.1_0": "Pañcaṅgiko maggoti?",
  "kv20.5:10.2_0": "Āmantā.",
  "kv20.5:10.3_0": "Nanu vuttaṁ bhagavatā—",
  "kv20.5:10.4_0": "“yasmiṁ kho, subhadda,",
  "kv20.5:10.4_1": "dhammavinaye ariyo aṭṭhaṅgiko maggo na upalabbhati,",
  "kv20.5:10.4_2": "samaṇopi tattha na upalabbhati,",
  "kv20.5:10.4_3": "dutiyopi tattha samaṇo na upalabbhati,",
  "kv20.5:10.4_4": "tatiyopi tattha samaṇo na upalabbhati,",
  "kv20.5:10.4_5": "catutthopi tattha samaṇo na upalabbhati.",
  "kv20.5:10.5_0": "Yasmiñca kho, subhadda,",
  "kv20.5:10.5_1": "dhammavinaye ariyo aṭṭhaṅgiko maggo upalabbhati, samaṇopi tattha upalabbhati,",
  "kv20.5:10.5_2": "dutiyopi …pe… tatiyopi …pe… catutthopi tattha samaṇo upalabbhati.",
  "kv20.5:10.6_0": "Imasmiṁ kho, subhadda,",
  "kv20.5:10.6_1": "dhammavinaye ariyo aṭṭhaṅgiko maggo upalabbhati. Idheva, subhadda, samaṇo, idha dutiyo samaṇo, idha tatiyo samaṇo, idha catuttho samaṇo.",
  "kv20.5:10.6_2": "Suññā parappavādā samaṇebhi aññehī”ti.",
  "kv20.5:10.7_0": "Attheva suttantoti?",
  "kv20.5:10.8_0": "Āmantā.",
  "kv20.5:10.9_0": "Tena hi aṭṭhaṅgiko maggoti.",
  "kv20.5:10.10_0": "Maggakathā niṭṭhitā."
}