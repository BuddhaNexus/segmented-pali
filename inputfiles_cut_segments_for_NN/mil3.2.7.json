{
  "mil3.2.7:0.1_0": "Milindapañha",
  "mil3.2.7:0.2_0": "Addhānavagga",
  "mil3.2.7:0.3_0": "7. Therapaṭisandahanāpaṭisandahanapañha",
  "mil3.2.7:1.1_0": "Rājā āha—",
  "mil3.2.7:1.2_0": "“bhante nāgasena, tvaṁ pana paṭisandahissasī”ti?",
  "mil3.2.7:1.3_0": "“Alaṁ, mahārāja,",
  "mil3.2.7:1.3_1": "kiṁ te tena pucchitena,",
  "mil3.2.7:1.3_2": "nanu mayā paṭikacceva akkhātaṁ ‘sace, mahārāja,",
  "mil3.2.7:1.3_3": "saupādāno bhavissāmi,",
  "mil3.2.7:1.3_4": "paṭisandahissāmi,",
  "mil3.2.7:1.3_5": "sace anupādāno bhavissāmi,",
  "mil3.2.7:1.3_6": "na paṭisandahissāmī’”ti.",
  "mil3.2.7:2.1_0": "“Opammaṁ karohī”ti.",
  "mil3.2.7:2.2_0": "“Yathā, mahārāja, kocideva puriso rañño adhikāraṁ kareyya.",
  "mil3.2.7:2.3_0": "Rājā tuṭṭho adhikāraṁ dadeyya,",
  "mil3.2.7:2.3_1": "so tena adhikārena pañcahi kāmaguṇehi samappito samaṅgibhūto paricareyya,",
  "mil3.2.7:2.3_2": "so ce janassa āroceyya ‘na me rājā kiñci paṭikarotī’ti.",
  "mil3.2.7:2.4_0": "Kiṁ nu kho so, mahārāja, puriso yuttakārī bhaveyyā”ti?",
  "mil3.2.7:2.5_0": "“Na hi, bhante”ti.",
  "mil3.2.7:2.6_0": "“Evameva kho, mahārāja,",
  "mil3.2.7:2.6_1": "kiṁ te tena pucchitena,",
  "mil3.2.7:2.6_2": "nanu mayā paṭikacceva akkhātaṁ ‘sace saupādāno bhavissāmi,",
  "mil3.2.7:2.6_3": "paṭisandahissāmi,",
  "mil3.2.7:2.6_4": "sace anupādāno bhavissāmi,",
  "mil3.2.7:2.6_5": "na paṭisandahissāmī’”ti.",
  "mil3.2.7:3.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.2.7:4.1_0": "Therapaṭisandahanāpaṭisandahanapañho sattamo."
}