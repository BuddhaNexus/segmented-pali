{
  "ja429:0.1_0": "Jātaka",
  "ja429:0.2_0": "Navakanipāta",
  "ja429:0.3_0": "Gijjhavagga",
  "ja429:0.4_0": "3. Mahāsuvajātaka",
  "ja429:1.1_0": "“Dumo yadā hoti phalūpapanno,",
  "ja429:1.2_0": "Bhuñjanti naṁ vihaṅgamā sampatantā;",
  "ja429:1.3_0": "Khīṇanti ñatvāna dumaṁ phalaccaye,",
  "ja429:1.4_0": "Disodisaṁ yanti tato vihaṅgamā.",
  "ja429:2.1_0": "Cara cārikaṁ lohitatuṇḍa māmari,",
  "ja429:2.2_0": "Kiṁ tvaṁ suva sukkhadumamhi jhāyasi;",
  "ja429:2.3_0": "Tadiṅgha maṁ brūhi vasantasannibha,",
  "ja429:2.4_0": "Kasmā suva sukkhadumaṁ na riñcasi”.",
  "ja429:3.1_0": "“Ye ve sakhīnaṁ sakhāro bhavanti,",
  "ja429:3.2_0": "Pāṇaccaye dukkhasukhesu haṁsa;",
  "ja429:3.3_0": "Khīṇaṁ akhīṇampi na taṁ jahanti,",
  "ja429:3.4_0": "Santo sataṁ dhammamanussarantā.",
  "ja429:4.1_0": "Sohaṁ sataṁ aññatarosmi haṁsa,",
  "ja429:4.2_0": "Ñātī ca me hoti sakhā ca rukkho;",
  "ja429:4.3_0": "Taṁ nussahe jīvikattho pahātuṁ,",
  "ja429:4.4_0": "Khīṇanti ñatvāna na hesa dhammo”.",
  "ja429:5.1_0": "“Sādhu sakkhi kataṁ hoti,",
  "ja429:5.2_0": "metti saṁsati santhavo;",
  "ja429:5.3_0": "Sacetaṁ dhammaṁ rocesi,",
  "ja429:5.4_0": "pāsaṁsosi vijānataṁ.",
  "ja429:6.1_0": "So te suva varaṁ dammi,",
  "ja429:6.2_0": "pattayāna vihaṅgama;",
  "ja429:6.3_0": "Varaṁ varassu vakkaṅga,",
  "ja429:6.4_0": "yaṁ kiñci manasicchasi”.",
  "ja429:7.1_0": "“Varañca me haṁsa bhavaṁ dadeyya,",
  "ja429:7.2_0": "Ayañca rukkho punarāyuṁ labhetha;",
  "ja429:7.3_0": "So sākhavā phalimā saṁvirūḷho,",
  "ja429:7.4_0": "Madhutthiko tiṭṭhatu sobhamāno”.",
  "ja429:8.1_0": "“Taṁ passa samma phalimaṁ uḷāraṁ,",
  "ja429:8.2_0": "Sahāva te hotu udumbarena;",
  "ja429:8.3_0": "So sākhavā phalimā saṁvirūḷho,",
  "ja429:8.4_0": "Madhutthiko tiṭṭhatu sobhamāno”.",
  "ja429:9.1_0": "“Evaṁ sakka sukhī hohi,",
  "ja429:9.2_0": "saha sabbehi ñātibhi;",
  "ja429:9.3_0": "Yathāhamajja sukhito,",
  "ja429:9.4_0": "disvāna saphalaṁ dumaṁ”.",
  "ja429:10.1_0": "“Suvassa ca varaṁ datvā,",
  "ja429:10.2_0": "katvāna saphalaṁ dumaṁ;",
  "ja429:10.3_0": "Pakkāmi saha bhariyāya,",
  "ja429:10.4_0": "devānaṁ nandanaṁ vanan”ti.",
  "ja429:11.1_0": "Mahāsuvajātakaṁ tatiyaṁ."
}