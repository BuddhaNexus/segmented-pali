{
  "tha-ap470:0.1_0": "Therāpadāna",
  "tha-ap470:0.2_0": "Sālakusumiyavagga",
  "tha-ap470:0.3_0": "8 Pilakkhaphaladāyakattheraapadāna",
  "tha-ap470:1.1_0": "“Vanantare buddhaṁ disvā,",
  "tha-ap470:1.2_0": "atthadassiṁ mahāyasaṁ;",
  "tha-ap470:1.3_0": "Pasannacitto sumano,",
  "tha-ap470:1.4_0": "pilakkhassa phalaṁ adā.",
  "tha-ap470:2.1_0": "Aṭṭhārase kappasate,",
  "tha-ap470:2.2_0": "yaṁ phalaṁ adadiṁ tadā;",
  "tha-ap470:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap470:2.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap470:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap470:3.2_0": "…pe…",
  "tha-ap470:3.3_0": "viharāmi anāsavo.",
  "tha-ap470:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap470:4.2_0": "…pe…",
  "tha-ap470:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap470:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap470:5.2_0": "…pe…",
  "tha-ap470:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap470:6.1_0": "Itthaṁ sudaṁ āyasmā pilakkhaphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap470:7.1_0": "Pilakkhaphaladāyakattherassāpadānaṁ aṭṭhamaṁ."
}