{
  "iti19:0.1_0": "Itivuttaka 19",
  "iti19:0.2_0": "Ekakanipāta",
  "iti19:0.3_0": "Dutiyavagga",
  "iti19:0.4_0": "Saṅghasāmaggīsutta",
  "iti19:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti19:2.1_0": "“Ekadhammo, bhikkhave,",
  "iti19:2.1_1": "loke uppajjamāno uppajjati bahujanahitāya bahujanasukhāya bahuno janassa atthāya hitāya sukhāya devamanussānaṁ.",
  "iti19:2.2_0": "Katamo ekadhammo?",
  "iti19:2.3_0": "Saṅghasāmaggī.",
  "iti19:2.4_0": "Saṅghe kho pana, bhikkhave,",
  "iti19:2.4_1": "samagge na ceva aññamaññaṁ bhaṇḍanāni honti,",
  "iti19:2.4_2": "na ca aññamaññaṁ paribhāsā honti,",
  "iti19:2.4_3": "na ca aññamaññaṁ parikkhepā honti,",
  "iti19:2.4_4": "na ca aññamaññaṁ pariccajanā honti.",
  "iti19:2.5_0": "Tattha appasannā ceva pasīdanti,",
  "iti19:2.5_1": "pasannānañca bhiyyobhāvo hotī”ti.",
  "iti19:2.6_0": "Etamatthaṁ bhagavā avoca.",
  "iti19:2.7_0": "Tatthetaṁ iti vuccati:",
  "iti19:3.1_0": "“Sukhā saṅghassa sāmaggī,",
  "iti19:3.2_0": "samaggānañcanuggaho;",
  "iti19:3.3_0": "Samaggarato dhammaṭṭho,",
  "iti19:3.4_0": "yogakkhemā na dhaṁsati;",
  "iti19:3.5_0": "Saṅghaṁ samaggaṁ katvāna,",
  "iti19:3.6_0": "kappaṁ saggamhi modatī”ti.",
  "iti19:4.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti19:4.2_0": "Navamaṁ."
}