{
  "iti8:0.1_0": "Itivuttaka 8",
  "iti8:0.2_0": "Ekakanipāta",
  "iti8:0.3_0": "Paṭhamavagga",
  "iti8:0.4_0": "Mānapariññāsutta",
  "iti8:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti8:2.1_0": "“Mānaṁ, bhikkhave,",
  "iti8:2.1_1": "anabhijānaṁ aparijānaṁ tattha cittaṁ avirājayaṁ appajahaṁ abhabbo dukkhakkhayāya.",
  "iti8:2.2_0": "Mānañca kho, bhikkhave,",
  "iti8:2.2_1": "abhijānaṁ parijānaṁ tattha cittaṁ virājayaṁ pajahaṁ bhabbo dukkhakkhayāyā”ti.",
  "iti8:2.3_0": "Etamatthaṁ bhagavā avoca.",
  "iti8:2.4_0": "Tatthetaṁ iti vuccati:",
  "iti8:3.1_0": "“Mānupetā ayaṁ pajā,",
  "iti8:3.2_0": "mānaganthā bhave ratā;",
  "iti8:3.3_0": "Mānaṁ aparijānantā,",
  "iti8:3.4_0": "āgantāro punabbhavaṁ.",
  "iti8:4.1_0": "Ye ca mānaṁ pahantvāna,",
  "iti8:4.2_0": "vimuttā mānasaṅkhaye;",
  "iti8:4.3_0": "Te mānaganthābhibhuno,",
  "iti8:4.4_0": "sabbadukkhamupaccagun”ti.",
  "iti8:5.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti8:5.2_0": "Aṭṭhamaṁ."
}