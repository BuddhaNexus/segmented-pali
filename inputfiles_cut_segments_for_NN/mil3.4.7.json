{
  "mil3.4.7:0.1_0": "Milindapañha",
  "mil3.4.7:0.2_0": "Nibbānavagga",
  "mil3.4.7:0.3_0": "7. Pathavisandhārakapañha",
  "mil3.4.7:1.1_0": "Rājā āha—",
  "mil3.4.7:1.2_0": "“bhante nāgasena, tumhe bhaṇatha—",
  "mil3.4.7:1.3_0": "‘ayaṁ mahā pathavī udake patiṭṭhitā, udakaṁ vāte patiṭṭhitaṁ, vāto ākāse patiṭṭhito’ti,",
  "mil3.4.7:1.3_1": "etampi vacanaṁ na saddahāmī”ti.",
  "mil3.4.7:1.4_0": "Thero dhammakarakena udakaṁ gahetvā rājānaṁ milindaṁ saññāpesi—",
  "mil3.4.7:1.5_0": "“yathā, mahārāja,",
  "mil3.4.7:1.5_1": "imaṁ udakaṁ vātena ādhāritaṁ,",
  "mil3.4.7:1.5_2": "evaṁ tampi udakaṁ vātena ādhāritan”ti.",
  "mil3.4.7:2.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.4.7:3.1_0": "Pathavisandhārakapañho sattamo."
}