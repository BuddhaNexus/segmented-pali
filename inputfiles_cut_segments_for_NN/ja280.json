{
  "ja280:0.1_0": "Jātaka",
  "ja280:0.2_0": "Tikanipāta",
  "ja280:0.3_0": "Udapānavagga",
  "ja280:0.4_0": "10. Puṭadūsakajātaka",
  "ja280:1.1_0": "“Addhā hi nūna migarājā,",
  "ja280:1.2_0": "puṭakammassa kovido;",
  "ja280:1.3_0": "Tathā hi puṭaṁ dūseti,",
  "ja280:1.4_0": "aññaṁ nūna karissati”.",
  "ja280:2.1_0": "“Na me mātā vā pitā vā,",
  "ja280:2.2_0": "puṭakammassa kovido;",
  "ja280:2.3_0": "Kataṁ kataṁ kho dūsema,",
  "ja280:2.4_0": "evaṁ dhammamidaṁ kulaṁ”.",
  "ja280:3.1_0": "“Yesaṁ vo ediso dhammo,",
  "ja280:3.2_0": "Adhammo pana kīdiso;",
  "ja280:3.3_0": "Mā vo dhammaṁ adhammaṁ vā,",
  "ja280:3.4_0": "Addasāma kudācanan”ti.",
  "ja280:4.1_0": "Puṭadūsakajātakaṁ dasamaṁ.",
  "ja280:4.2_0": "Udapānavaggo tatiyo.",
  "ja280:5.1_0": "Tassuddānaṁ",
  "ja280:6.1_0": "Udapānavaraṁ vanabyaggha kapi,",
  "ja280:6.2_0": "Sikhinī ca balāka ruciravaro;",
  "ja280:6.3_0": "Sujanādhiparomakadūsa puna,",
  "ja280:6.4_0": "Satapattavaro puṭakamma dasāti."
}