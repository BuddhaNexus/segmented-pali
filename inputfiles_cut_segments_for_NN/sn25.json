{
  "sn25:1.0.1_0": "Saṁyutta Nikāya 25 ",
  "sn25:1.0.2_0": "1. Cakkhuvagga ",
  "sn25:1.0.3_0": "1. Cakkhusutta ",
  "sn25:1.1.1_0": "Sāvatthinidānaṁ. ",
  "sn25:1.1.2_0": "“Cakkhuṁ, bhikkhave, aniccaṁ vipariṇāmi aññathābhāvi; ",
  "sn25:1.1.3_0": "sotaṁ aniccaṁ vipariṇāmi aññathābhāvi; ",
  "sn25:1.1.4_0": "ghānaṁ aniccaṁ vipariṇāmi aññathābhāvi; ",
  "sn25:1.1.5_0": "jivhā aniccā vipariṇāmī aññathābhāvī; ",
  "sn25:1.1.6_0": "kāyo anicco vipariṇāmi aññathābhāvi; ",
  "sn25:1.1.7_0": "mano anicco vipariṇāmī aññathābhāvī. ",
  "sn25:1.1.8_0": "Yo,",
  "sn25:1.1.8_1": "bhikkhave,",
  "sn25:1.1.8_2": "ime dhamme evaṁ saddahati adhimuccati—ayaṁ vuccati saddhānusārī,",
  "sn25:1.1.8_3": "okkanto sammattaniyāmaṁ,",
  "sn25:1.1.8_4": "sappurisabhūmiṁ okkanto,",
  "sn25:1.1.8_5": "vītivatto puthujjanabhūmiṁ;",
  "sn25:1.1.9_0": "abhabbo taṁ kammaṁ kātuṁ,",
  "sn25:1.1.9_1": "yaṁ kammaṁ katvā nirayaṁ vā tiracchānayoniṁ vā pettivisayaṁ vā upapajjeyya;",
  "sn25:1.1.10_0": "abhabbo ca tāva kālaṁ kātuṁ yāva na sotāpattiphalaṁ sacchikaroti. ",
  "sn25:1.2.1_0": "Yassa kho,",
  "sn25:1.2.1_1": "bhikkhave,",
  "sn25:1.2.1_2": "ime dhammā evaṁ paññāya mattaso nijjhānaṁ khamanti,",
  "sn25:1.2.1_3": "ayaṁ vuccati:",
  "sn25:1.2.1_4": "‘dhammānusārī,",
  "sn25:1.2.1_5": "okkanto sammattaniyāmaṁ,",
  "sn25:1.2.1_6": "sappurisabhūmiṁ okkanto,",
  "sn25:1.2.1_7": "vītivatto puthujjanabhūmiṁ;",
  "sn25:1.2.2_0": "abhabbo taṁ kammaṁ kātuṁ,",
  "sn25:1.2.2_1": "yaṁ kammaṁ katvā nirayaṁ vā tiracchānayoniṁ vā pettivisayaṁ vā upapajjeyya;",
  "sn25:1.2.3_0": "abhabbo ca tāva kālaṁ kātuṁ yāva na sotāpattiphalaṁ sacchikaroti’. ",
  "sn25:1.2.4_0": "Yo,",
  "sn25:1.2.4_1": "bhikkhave,",
  "sn25:1.2.4_2": "ime dhamme evaṁ pajānāti evaṁ passati,",
  "sn25:1.2.4_3": "ayaṁ vuccati:",
  "sn25:1.2.4_4": "‘sotāpanno avinipātadhammo niyato sambodhiparāyano’”ti.",
  "sn25:1.2.5_0": "Paṭhamaṁ. ",
  "sn25:2.0.1_0": "Saṁyutta Nikāya 25 ",
  "sn25:2.0.2_0": "1. Cakkhuvagga ",
  "sn25:2.0.3_0": "2. Rūpasutta ",
  "sn25:2.1.1_0": "Sāvatthinidānaṁ. ",
  "sn25:2.1.2_0": "“Rūpā, bhikkhave, aniccā vipariṇāmino aññathābhāvino; ",
  "sn25:2.1.3_0": "saddā aniccā vipariṇāmino aññathābhāvino; ",
  "sn25:2.1.4_0": "gandhā aniccā vipariṇāmino aññathābhāvino; ",
  "sn25:2.1.5_0": "rasā aniccā vipariṇāmino aññathābhāvino; ",
  "sn25:2.1.6_0": "phoṭṭhabbā aniccā vipariṇāmino aññathābhāvino; ",
  "sn25:2.1.7_0": "dhammā aniccā vipariṇāmino aññathābhāvino. ",
  "sn25:2.1.8_0": "Yo,",
  "sn25:2.1.8_1": "bhikkhave,",
  "sn25:2.1.8_2": "ime dhamme evaṁ saddahati adhimuccati,",
  "sn25:2.1.8_3": "ayaṁ vuccati saddhānusārī,",
  "sn25:2.1.8_4": "okkanto sammattaniyāmaṁ,",
  "sn25:2.1.8_5": "sappurisabhūmiṁ okkanto,",
  "sn25:2.1.8_6": "vītivatto puthujjanabhūmiṁ;",
  "sn25:2.1.9_0": "abhabbo taṁ kammaṁ kātuṁ,",
  "sn25:2.1.9_1": "yaṁ kammaṁ katvā nirayaṁ vā tiracchānayoniṁ vā pettivisayaṁ vā upapajjeyya;",
  "sn25:2.1.10_0": "abhabbo ca tāva kālaṁ kātuṁ yāva na sotāpattiphalaṁ sacchikaroti. ",
  "sn25:2.2.1_0": "Yassa kho,",
  "sn25:2.2.1_1": "bhikkhave,",
  "sn25:2.2.1_2": "ime dhammā evaṁ paññāya mattaso nijjhānaṁ khamanti,",
  "sn25:2.2.1_3": "ayaṁ vuccati:",
  "sn25:2.2.1_4": "‘dhammānusārī,",
  "sn25:2.2.1_5": "okkanto sammattaniyāmaṁ,",
  "sn25:2.2.1_6": "sappurisabhūmiṁ okkanto,",
  "sn25:2.2.1_7": "vītivatto puthujjanabhūmiṁ;",
  "sn25:2.2.2_0": "abhabbo taṁ kammaṁ kātuṁ,",
  "sn25:2.2.2_1": "yaṁ kammaṁ katvā nirayaṁ vā tiracchānayoniṁ vā pettivisayaṁ vā upapajjeyya;",
  "sn25:2.2.3_0": "abhabbo ca tāva kālaṁ kātuṁ yāva na sotāpattiphalaṁ sacchikaroti’. ",
  "sn25:2.2.4_0": "Yo, bhikkhave, ime dhamme evaṁ pajānāti evaṁ passati, ayaṁ vuccati: ",
  "sn25:2.2.5_0": "‘sotāpanno avinipātadhammo niyato sambodhiparāyano’”ti. ",
  "sn25:2.2.6_0": "Dutiyaṁ. ",
  "sn25:3.0.1_0": "Saṁyutta Nikāya 25 ",
  "sn25:3.0.2_0": "1. Cakkhuvagga ",
  "sn25:3.0.3_0": "3. Viññāṇasutta ",
  "sn25:3.1.1_0": "Sāvatthinidānaṁ. ",
  "sn25:3.1.2_0": "“Cakkhuviññāṇaṁ, bhikkhave, aniccaṁ vipariṇāmi aññathābhāvi; ",
  "sn25:3.1.3_0": "sotaviññāṇaṁ … ",
  "sn25:3.1.4_0": "ghānaviññāṇaṁ … ",
  "sn25:3.1.5_0": "jivhāviññāṇaṁ … ",
  "sn25:3.1.6_0": "kāyaviññāṇaṁ … ",
  "sn25:3.1.7_0": "manoviññāṇaṁ aniccaṁ vipariṇāmi aññathābhāvi. ",
  "sn25:3.1.8_0": "Yo bhikkhave …pe… ",
  "sn25:3.1.9_0": "sambodhiparāyano”ti. ",
  "sn25:3.1.10_0": "Tatiyaṁ. ",
  "sn25:4.0.1_0": "Saṁyutta Nikāya 25 ",
  "sn25:4.0.2_0": "1. Cakkhuvagga ",
  "sn25:4.0.3_0": "4. Samphassasutta ",
  "sn25:4.1.1_0": "Sāvatthinidānaṁ. ",
  "sn25:4.1.2_0": "“Cakkhusamphasso, bhikkhave, anicco vipariṇāmī aññathābhāvī; ",
  "sn25:4.1.3_0": "sotasamphasso … ",
  "sn25:4.1.4_0": "ghānasamphasso … ",
  "sn25:4.1.5_0": "jivhāsamphasso … ",
  "sn25:4.1.6_0": "kāyasamphasso … ",
  "sn25:4.1.7_0": "manosamphasso anicco vipariṇāmī aññathābhāvī. ",
  "sn25:4.1.8_0": "Yo, bhikkhave, ime dhamme evaṁ saddahati adhimuccati, ayaṁ vuccati ‘saddhānusārī …pe… ",
  "sn25:4.1.9_0": "sambodhiparāyano’”ti. ",
  "sn25:4.1.10_0": "Catutthaṁ. ",
  "sn25:5.0.1_0": "Saṁyutta Nikāya 25 ",
  "sn25:5.0.2_0": "1. Cakkhuvagga ",
  "sn25:5.0.3_0": "5. Samphassajasutta ",
  "sn25:5.1.1_0": "Sāvatthinidānaṁ. ",
  "sn25:5.1.2_0": "“Cakkhusamphassajā, bhikkhave, vedanā aniccā vipariṇāmī aññathābhāvī; ",
  "sn25:5.1.3_0": "sotasamphassajā vedanā …pe… ",
  "sn25:5.1.4_0": "ghānasamphassajā vedanā …pe… ",
  "sn25:5.1.5_0": "jivhāsamphassajā vedanā …pe… ",
  "sn25:5.1.6_0": "kāyasamphassajā vedanā …pe… ",
  "sn25:5.1.7_0": "manosamphassajā vedanā aniccā vipariṇāmī aññathābhāvī. ",
  "sn25:5.1.8_0": "Yo, bhikkhave, ime dhamme evaṁ saddahati adhimuccati, ayaṁ vuccati ‘saddhānusārī …pe… ",
  "sn25:5.1.9_0": "sambodhiparāyano’”ti. ",
  "sn25:5.1.10_0": "Pañcamaṁ. ",
  "sn25:6.0.1_0": "Saṁyutta Nikāya 25 ",
  "sn25:6.0.2_0": "1. Cakkhuvagga ",
  "sn25:6.0.3_0": "6. Rūpasaññāsutta ",
  "sn25:6.1.1_0": "Sāvatthinidānaṁ. ",
  "sn25:6.1.2_0": "“Rūpasaññā, bhikkhave, aniccā vipariṇāmī aññathābhāvī; ",
  "sn25:6.1.3_0": "saddasaññā … ",
  "sn25:6.1.4_0": "gandhasaññā … ",
  "sn25:6.1.5_0": "rasasaññā … ",
  "sn25:6.1.6_0": "phoṭṭhabbasaññā … ",
  "sn25:6.1.7_0": "dhammasaññā aniccā vipariṇāmī aññathābhāvī. ",
  "sn25:6.1.8_0": "Yo, bhikkhave, ime dhamme evaṁ saddahati adhimuccati, ayaṁ vuccati ‘saddhānusārī …pe… ",
  "sn25:6.1.9_0": "sambodhiparāyano’”ti. ",
  "sn25:6.1.10_0": "Chaṭṭhaṁ. ",
  "sn25:7.0.1_0": "Saṁyutta Nikāya 25 ",
  "sn25:7.0.2_0": "1. Cakkhuvagga ",
  "sn25:7.0.3_0": "7. Rūpasañcetanāsutta ",
  "sn25:7.1.1_0": "Sāvatthinidānaṁ. ",
  "sn25:7.1.2_0": "“Rūpasañcetanā, bhikkhave, aniccā vipariṇāmī aññathābhāvī; ",
  "sn25:7.1.3_0": "saddasañcetanā … ",
  "sn25:7.1.4_0": "gandhasañcetanā … ",
  "sn25:7.1.5_0": "rasasañcetanā … ",
  "sn25:7.1.6_0": "phoṭṭhabbasañcetanā … ",
  "sn25:7.1.7_0": "dhammasañcetanā aniccā vipariṇāmī aññathābhāvī. ",
  "sn25:7.1.8_0": "Yo, bhikkhave, ime dhamme evaṁ saddahati adhimuccati, ayaṁ vuccati ‘saddhānusārī …pe… ",
  "sn25:7.1.9_0": "sambodhiparāyano’”ti. ",
  "sn25:7.1.10_0": "Sattamaṁ. ",
  "sn25:8.0.1_0": "Saṁyutta Nikāya 25 ",
  "sn25:8.0.2_0": "1. Cakkhuvagga ",
  "sn25:8.0.3_0": "8. Rūpataṇhāsutta ",
  "sn25:8.1.1_0": "Sāvatthinidānaṁ. ",
  "sn25:8.1.2_0": "“Rūpataṇhā, bhikkhave, aniccā vipariṇāmī aññathābhāvī; ",
  "sn25:8.1.3_0": "saddataṇhā … ",
  "sn25:8.1.4_0": "gandhataṇhā … ",
  "sn25:8.1.5_0": "rasataṇhā … ",
  "sn25:8.1.6_0": "phoṭṭhabbataṇhā … ",
  "sn25:8.1.7_0": "dhammataṇhā aniccā vipariṇāmī aññathābhāvī. ",
  "sn25:8.1.8_0": "Yo, bhikkhave, ime dhamme evaṁ saddahati adhimuccati, ayaṁ vuccati ‘saddhānusārī …pe… ",
  "sn25:8.1.9_0": "sambodhiparāyano’”ti. ",
  "sn25:8.1.10_0": "Aṭṭhamaṁ. ",
  "sn25:9.0.1_0": "Saṁyutta Nikāya 25 ",
  "sn25:9.0.2_0": "1. Cakkhuvagga ",
  "sn25:9.0.3_0": "9. Pathavīdhātusutta ",
  "sn25:9.1.1_0": "Sāvatthinidānaṁ. ",
  "sn25:9.1.2_0": "“Pathavīdhātu, bhikkhave, aniccā vipariṇāmī aññathābhāvī; ",
  "sn25:9.1.3_0": "āpodhātu … ",
  "sn25:9.1.4_0": "tejodhātu … ",
  "sn25:9.1.5_0": "vāyodhātu … ",
  "sn25:9.1.6_0": "ākāsadhātu … ",
  "sn25:9.1.7_0": "viññāṇadhātu aniccā vipariṇāmī aññathābhāvī. ",
  "sn25:9.1.8_0": "Yo, bhikkhave, ime dhamme evaṁ saddahati adhimuccati, ayaṁ vuccati ‘saddhānusārī …pe… ",
  "sn25:9.1.9_0": "sambodhiparāyano’”ti. ",
  "sn25:9.1.10_0": "Navamaṁ. ",
  "sn25:10.0.1_0": "Saṁyutta Nikāya 25 ",
  "sn25:10.0.2_0": "1. Cakkhuvagga ",
  "sn25:10.0.3_0": "10. Khandhasutta ",
  "sn25:10.1.1_0": "Sāvatthinidānaṁ. ",
  "sn25:10.1.2_0": "“Rūpaṁ, bhikkhave, aniccaṁ vipariṇāmi aññathābhāvi; ",
  "sn25:10.1.3_0": "vedanā aniccā vipariṇāmī aññathābhāvī; ",
  "sn25:10.1.4_0": "saññā … ",
  "sn25:10.1.5_0": "saṅkhārā aniccā vipariṇāmino aññathābhāvino; ",
  "sn25:10.1.6_0": "viññāṇaṁ aniccaṁ vipariṇāmi aññathābhāvi. ",
  "sn25:10.1.7_0": "Yo,",
  "sn25:10.1.7_1": "bhikkhave,",
  "sn25:10.1.7_2": "ime dhamme evaṁ saddahati adhimuccati,",
  "sn25:10.1.7_3": "ayaṁ vuccati saddhānusārī,",
  "sn25:10.1.7_4": "okkanto sammattaniyāmaṁ,",
  "sn25:10.1.7_5": "sappurisabhūmiṁ okkanto,",
  "sn25:10.1.7_6": "vītivatto puthujjanabhūmiṁ;",
  "sn25:10.1.8_0": "abhabbo taṁ kammaṁ kātuṁ,",
  "sn25:10.1.8_1": "yaṁ kammaṁ katvā nirayaṁ vā tiracchānayoniṁ vā pettivisayaṁ vā upapajjeyya;",
  "sn25:10.1.9_0": "abhabbo ca tāva kālaṁ kātuṁ yāva na sotāpattiphalaṁ sacchikaroti. ",
  "sn25:10.2.1_0": "Yassa kho,",
  "sn25:10.2.1_1": "bhikkhave,",
  "sn25:10.2.1_2": "ime dhammā evaṁ paññāya mattaso nijjhānaṁ khamanti,",
  "sn25:10.2.1_3": "ayaṁ vuccati:",
  "sn25:10.2.1_4": "‘dhammānusārī,",
  "sn25:10.2.1_5": "okkanto sammattaniyāmaṁ,",
  "sn25:10.2.1_6": "sappurisabhūmiṁ okkanto,",
  "sn25:10.2.1_7": "vītivatto puthujjanabhūmiṁ;",
  "sn25:10.2.2_0": "abhabbo taṁ kammaṁ kātuṁ,",
  "sn25:10.2.2_1": "yaṁ kammaṁ katvā nirayaṁ vā tiracchānayoniṁ vā pettivisayaṁ vā upapajjeyya;",
  "sn25:10.2.3_0": "abhabbo ca tāva kālaṁ kātuṁ yāva na sotāpattiphalaṁ sacchikaroti’. ",
  "sn25:10.2.4_0": "Yo, bhikkhave, ime dhamme evaṁ pajānāti evaṁ passati, ayaṁ vuccati: ",
  "sn25:10.2.5_0": "‘sotāpanno avinipātadhammo niyato sambodhiparāyano’”ti. ",
  "sn25:10.2.6_0": "Dasamaṁ. ",
  "sn25:10.2.7_0": "Cakkhuvaggo paṭhamo. ",
  "sn25:10.3.0_0": "Tassuddānaṁ",
  "sn25:10.3.1_0": "Cakkhu rūpañca viññāṇaṁ, ",
  "sn25:10.3.2_0": "phasso ca vedanāya ca; ",
  "sn25:10.3.3_0": "Saññā ca cetanā taṇhā, ",
  "sn25:10.3.4_0": "dhātu khandhena te dasāti. ",
  "sn25:10.3.5_0": "Okkantasaṁyuttaṁ samattaṁ. "
}