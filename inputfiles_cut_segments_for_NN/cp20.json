{
  "cp20:0.1_0": "Cariyāpiṭaka",
  "cp20:0.2_0": "Hatthināgavagga",
  "cp20:0.3_0": "Sīlapāramī 10",
  "cp20:0.4_0": "Saṅkhapālacariya",
  "cp20:1.1_0": "“Punāparaṁ yadā homi,",
  "cp20:1.2_0": "saṅkhapālo mahiddhiko;",
  "cp20:1.3_0": "Dāṭhāvudho ghoraviso,",
  "cp20:1.4_0": "dvijivho uragādhibhū.",
  "cp20:2.1_0": "Catuppathe mahāmagge,",
  "cp20:2.2_0": "nānājanasamākule;",
  "cp20:2.3_0": "Caturo aṅge adhiṭṭhāya,",
  "cp20:2.4_0": "tattha vāsamakappayiṁ.",
  "cp20:3.1_0": "Chaviyā cammena maṁsena,",
  "cp20:3.2_0": "Nahāruaṭṭhikehi vā;",
  "cp20:3.3_0": "Yassa etena karaṇīyaṁ,",
  "cp20:3.4_0": "Dinnaṁyeva harātu so.",
  "cp20:4.1_0": "Addasaṁsu bhojaputtā,",
  "cp20:4.2_0": "kharā luddā akāruṇā;",
  "cp20:4.3_0": "Upagañchuṁ mamaṁ tattha,",
  "cp20:4.4_0": "daṇḍamuggarapāṇino.",
  "cp20:5.1_0": "Nāsāya vinivijjhitvā,",
  "cp20:5.2_0": "naṅguṭṭhe piṭṭhikaṇṭake;",
  "cp20:5.3_0": "Kāje āropayitvāna,",
  "cp20:5.4_0": "bhojaputtā hariṁsu maṁ.",
  "cp20:6.1_0": "Sasāgarantaṁ pathaviṁ,",
  "cp20:6.2_0": "sakānanaṁ sapabbataṁ;",
  "cp20:6.3_0": "Icchamāno cahaṁ tattha,",
  "cp20:6.4_0": "nāsāvātena jhāpaye.",
  "cp20:7.1_0": "Sūlehi vinivijjhante,",
  "cp20:7.2_0": "Koṭṭayantepi sattibhi;",
  "cp20:7.3_0": "Bhojaputte na kuppāmi,",
  "cp20:7.4_0": "Esā me sīlapāramī”ti. ",
  "cp20:8.1_0": "Saṅkhapālacariyaṁ dasamaṁ.",
  "cp20:9.1_0": "Hatthināgavaggo dutiyo.",
  "cp20:10.1_0": "Tassuddānaṁ",
  "cp20:11.1_0": "Hatthināgo bhūridatto,",
  "cp20:11.2_0": "campeyyo bodhi mahiṁso;",
  "cp20:11.3_0": "Ruru mātaṅgo dhammo ca,",
  "cp20:11.4_0": "atrajo ca jayaddiso.",
  "cp20:12.1_0": "Ete nava sīlabalā,",
  "cp20:12.2_0": "parikkhārā padesikā;",
  "cp20:12.3_0": "Jīvitaṁ parirakkhitvā,",
  "cp20:12.4_0": "sīlāni anurakkhisaṁ.",
  "cp20:13.1_0": "Saṅkhapālassa me sato,",
  "cp20:13.2_0": "sabbakālampi jīvitaṁ;",
  "cp20:13.3_0": "Yassa kassaci niyyattaṁ,",
  "cp20:13.4_0": "tasmā sā sīlapāramīti.",
  "cp20:14.1_0": "Sīlapāraminiddeso niṭṭhito."
}