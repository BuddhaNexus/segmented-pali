{
  "mn110:0.1_0": "Majjhima Nikāya 110",
  "mn110:0.2_0": "Cūḷapuṇṇamasutta",
  "mn110:1.1_0": "Evaṁ me sutaṁ—",
  "mn110:1.2_0": "ekaṁ samayaṁ bhagavā sāvatthiyaṁ viharati pubbārāme migāramātupāsāde.",
  "mn110:2.1_0": "Tena kho pana samayena bhagavā tadahuposathe pannarase puṇṇāya puṇṇamāya rattiyā bhikkhusaṅghaparivuto abbhokāse nisinno hoti.",
  "mn110:2.2_0": "Atha kho bhagavā tuṇhībhūtaṁ tuṇhībhūtaṁ bhikkhusaṅghaṁ anuviloketvā bhikkhū āmantesi:",
  "mn110:3.1_0": "“jāneyya nu kho, bhikkhave, asappuriso asappurisaṁ:",
  "mn110:3.2_0": "‘asappuriso ayaṁ bhavan’”ti?",
  "mn110:3.3_0": "“No hetaṁ, bhante”.",
  "mn110:3.4_0": "“Sādhu, bhikkhave;",
  "mn110:3.5_0": "aṭṭhānametaṁ, bhikkhave, anavakāso yaṁ asappuriso asappurisaṁ jāneyya:",
  "mn110:3.6_0": "‘asappuriso ayaṁ bhavan’ti.",
  "mn110:3.7_0": "Jāneyya pana, bhikkhave, asappuriso sappurisaṁ:",
  "mn110:3.8_0": "‘sappuriso ayaṁ bhavan’”ti?",
  "mn110:3.9_0": "“No hetaṁ, bhante”.",
  "mn110:3.10_0": "“Sādhu, bhikkhave;",
  "mn110:3.11_0": "etampi kho, bhikkhave, aṭṭhānaṁ anavakāso yaṁ asappuriso sappurisaṁ jāneyya:",
  "mn110:3.12_0": "‘sappuriso ayaṁ bhavan’ti.",
  "mn110:4.1_0": "Asappuriso,",
  "mn110:4.1_1": "bhikkhave,",
  "mn110:4.1_2": "assaddhammasamannāgato hoti,",
  "mn110:4.1_3": "asappurisabhatti hoti,",
  "mn110:4.1_4": "asappurisacintī hoti,",
  "mn110:4.1_5": "asappurisamantī hoti,",
  "mn110:4.1_6": "asappurisavāco hoti,",
  "mn110:4.1_7": "asappurisakammanto hoti,",
  "mn110:4.1_8": "asappurisadiṭṭhi hoti;",
  "mn110:4.1_9": "asappurisadānaṁ deti.",
  "mn110:5.1_0": "Kathañca, bhikkhave, asappuriso assaddhammasamannāgato hoti?",
  "mn110:5.2_0": "Idha,",
  "mn110:5.2_1": "bhikkhave,",
  "mn110:5.2_2": "asappuriso assaddho hoti,",
  "mn110:5.2_3": "ahiriko hoti,",
  "mn110:5.2_4": "anottappī hoti,",
  "mn110:5.2_5": "appassuto hoti,",
  "mn110:5.2_6": "kusīto hoti,",
  "mn110:5.2_7": "muṭṭhassati hoti,",
  "mn110:5.2_8": "duppañño hoti.",
  "mn110:5.3_0": "Evaṁ kho, bhikkhave, asappuriso assaddhammasamannāgato hoti.",
  "mn110:6.1_0": "Kathañca, bhikkhave, asappuriso asappurisabhatti hoti?",
  "mn110:6.2_0": "Idha,",
  "mn110:6.2_1": "bhikkhave,",
  "mn110:6.2_2": "asappurisassa ye te samaṇabrāhmaṇā assaddhā ahirikā anottappino appassutā kusītā muṭṭhassatino duppaññā tyāssa mittā honti te sahāyā.",
  "mn110:6.3_0": "Evaṁ kho, bhikkhave, asappuriso asappurisabhatti hoti.",
  "mn110:7.1_0": "Kathañca, bhikkhave, asappuriso asappurisacintī hoti?",
  "mn110:7.2_0": "Idha,",
  "mn110:7.2_1": "bhikkhave,",
  "mn110:7.2_2": "asappuriso attabyābādhāyapi ceteti,",
  "mn110:7.2_3": "parabyābādhāyapi ceteti,",
  "mn110:7.2_4": "ubhayabyābādhāyapi ceteti.",
  "mn110:7.3_0": "Evaṁ kho, bhikkhave, asappuriso asappurisacintī hoti.",
  "mn110:8.1_0": "Kathañca, bhikkhave, asappuriso asappurisamantī hoti?",
  "mn110:8.2_0": "Idha,",
  "mn110:8.2_1": "bhikkhave,",
  "mn110:8.2_2": "asappuriso attabyābādhāyapi manteti,",
  "mn110:8.2_3": "parabyābādhāyapi manteti,",
  "mn110:8.2_4": "ubhayabyābādhāyapi manteti.",
  "mn110:8.3_0": "Evaṁ kho, bhikkhave, asappuriso asappurisamantī hoti.",
  "mn110:9.1_0": "Kathañca, bhikkhave, asappuriso asappurisavāco hoti?",
  "mn110:9.2_0": "Idha, bhikkhave, asappuriso musāvādī hoti, pisuṇavāco hoti, pharusavāco hoti, samphappalāpī hoti.",
  "mn110:9.3_0": "Evaṁ kho, bhikkhave, asappuriso asappurisavāco hoti.",
  "mn110:10.1_0": "Kathañca, bhikkhave, asappuriso asappurisakammanto hoti?",
  "mn110:10.2_0": "Idha, bhikkhave, asappuriso pāṇātipātī hoti, adinnādāyī hoti, kāmesumicchācārī hoti.",
  "mn110:10.3_0": "Evaṁ kho, bhikkhave, asappuriso asappurisakammanto hoti.",
  "mn110:11.1_0": "Kathañca, bhikkhave, asappuriso asappurisadiṭṭhi hoti?",
  "mn110:11.2_0": "Idha, bhikkhave, asappuriso evaṁdiṭṭhi hoti:",
  "mn110:11.3_0": "‘natthi dinnaṁ,",
  "mn110:11.3_1": "natthi yiṭṭhaṁ,",
  "mn110:11.3_2": "natthi hutaṁ,",
  "mn110:11.3_3": "natthi sukatadukkaṭānaṁ kammānaṁ phalaṁ vipāko,",
  "mn110:11.3_4": "natthi ayaṁ loko,",
  "mn110:11.3_5": "natthi paro loko,",
  "mn110:11.3_6": "natthi mātā,",
  "mn110:11.3_7": "natthi pitā,",
  "mn110:11.3_8": "natthi sattā opapātikā,",
  "mn110:11.3_9": "natthi loke samaṇabrāhmaṇā sammaggatā sammāpaṭipannā,",
  "mn110:11.3_10": "ye imañca lokaṁ parañca lokaṁ sayaṁ abhiññā sacchikatvā pavedentī’ti.",
  "mn110:11.4_0": "Evaṁ kho, bhikkhave, asappuriso asappurisadiṭṭhi hoti.",
  "mn110:12.1_0": "Kathañca, bhikkhave, asappuriso asappurisadānaṁ deti?",
  "mn110:12.2_0": "Idha,",
  "mn110:12.2_1": "bhikkhave,",
  "mn110:12.2_2": "asappuriso asakkaccaṁ dānaṁ deti,",
  "mn110:12.2_3": "asahatthā dānaṁ deti,",
  "mn110:12.2_4": "acittīkatvā dānaṁ deti,",
  "mn110:12.2_5": "apaviṭṭhaṁ dānaṁ deti anāgamanadiṭṭhiko dānaṁ deti.",
  "mn110:12.3_0": "Evaṁ kho, bhikkhave, asappuriso asappurisadānaṁ deti.",
  "mn110:13.1_0": "So,",
  "mn110:13.1_1": "bhikkhave,",
  "mn110:13.1_2": "asappuriso evaṁ assaddhammasamannāgato,",
  "mn110:13.1_3": "evaṁ asappurisabhatti,",
  "mn110:13.1_4": "evaṁ asappurisacintī,",
  "mn110:13.1_5": "evaṁ asappurisamantī,",
  "mn110:13.1_6": "evaṁ asappurisavāco,",
  "mn110:13.1_7": "evaṁ asappurisakammanto,",
  "mn110:13.1_8": "evaṁ asappurisadiṭṭhi;",
  "mn110:13.2_0": "evaṁ asappurisadānaṁ datvā kāyassa bhedā paraṁ maraṇā yā asappurisānaṁ gati tattha upapajjati.",
  "mn110:13.3_0": "Kā ca, bhikkhave, asappurisānaṁ gati?",
  "mn110:13.4_0": "Nirayo vā tiracchānayoni vā.",
  "mn110:14.1_0": "Jāneyya nu kho, bhikkhave, sappuriso sappurisaṁ:",
  "mn110:14.2_0": "‘sappuriso ayaṁ bhavan’”ti?",
  "mn110:14.3_0": "“Evaṁ, bhante”.",
  "mn110:14.4_0": "“Sādhu, bhikkhave;",
  "mn110:14.5_0": "ṭhānametaṁ, bhikkhave, vijjati yaṁ sappuriso sappurisaṁ jāneyya:",
  "mn110:14.6_0": "‘sappuriso ayaṁ bhavan’ti.",
  "mn110:14.7_0": "Jāneyya pana, bhikkhave, sappuriso asappurisaṁ:",
  "mn110:14.8_0": "‘asappuriso ayaṁ bhavan’”ti?",
  "mn110:14.9_0": "“Evaṁ, bhante”.",
  "mn110:15.1_0": "“Sādhu, bhikkhave;",
  "mn110:15.2_0": "etampi kho, bhikkhave, ṭhānaṁ vijjati yaṁ sappuriso asappurisaṁ jāneyya:",
  "mn110:15.3_0": "‘asappuriso ayaṁ bhavan’ti.",
  "mn110:15.4_0": "Sappuriso,",
  "mn110:15.4_1": "bhikkhave,",
  "mn110:15.4_2": "saddhammasamannāgato hoti,",
  "mn110:15.4_3": "sappurisabhatti hoti,",
  "mn110:15.4_4": "sappurisacintī hoti,",
  "mn110:15.4_5": "sappurisamantī hoti,",
  "mn110:15.4_6": "sappurisavāco hoti,",
  "mn110:15.4_7": "sappurisakammanto hoti,",
  "mn110:15.4_8": "sappurisadiṭṭhi hoti;",
  "mn110:15.5_0": "sappurisadānaṁ deti.",
  "mn110:16.1_0": "Kathañca, bhikkhave, sappuriso saddhammasamannāgato hoti?",
  "mn110:16.2_0": "Idha,",
  "mn110:16.2_1": "bhikkhave,",
  "mn110:16.2_2": "sappuriso saddho hoti,",
  "mn110:16.2_3": "hirimā hoti,",
  "mn110:16.2_4": "ottappī hoti,",
  "mn110:16.2_5": "bahussuto hoti,",
  "mn110:16.2_6": "āraddhavīriyo hoti,",
  "mn110:16.2_7": "upaṭṭhitassati hoti,",
  "mn110:16.2_8": "paññavā hoti.",
  "mn110:16.3_0": "Evaṁ kho, bhikkhave, sappuriso saddhammasamannāgato hoti.",
  "mn110:17.1_0": "Kathañca, bhikkhave, sappuriso sappurisabhatti hoti?",
  "mn110:17.2_0": "Idha,",
  "mn110:17.2_1": "bhikkhave,",
  "mn110:17.2_2": "sappurisassa ye te samaṇabrāhmaṇā saddhā hirimanto ottappino bahussutā āraddhavīriyā upaṭṭhitassatino paññavanto tyāssa mittā honti,",
  "mn110:17.2_3": "te sahāyā.",
  "mn110:17.3_0": "Evaṁ kho, bhikkhave, sappuriso sappurisabhatti hoti.",
  "mn110:18.1_0": "Kathañca, bhikkhave, sappuriso sappurisacintī hoti?",
  "mn110:18.2_0": "Idha,",
  "mn110:18.2_1": "bhikkhave,",
  "mn110:18.2_2": "sappuriso nevattabyābādhāya ceteti,",
  "mn110:18.2_3": "na parabyābādhāya ceteti,",
  "mn110:18.2_4": "na ubhayabyābādhāya ceteti.",
  "mn110:18.3_0": "Evaṁ kho, bhikkhave, sappuriso sappurisacintī hoti.",
  "mn110:19.1_0": "Kathañca, bhikkhave, sappuriso sappurisamantī hoti?",
  "mn110:19.2_0": "Idha,",
  "mn110:19.2_1": "bhikkhave,",
  "mn110:19.2_2": "sappuriso nevattabyābādhāya manteti,",
  "mn110:19.2_3": "na parabyābādhāya manteti,",
  "mn110:19.2_4": "na ubhayabyābādhāya manteti.",
  "mn110:19.3_0": "Evaṁ kho, bhikkhave, sappuriso sappurisamantī hoti.",
  "mn110:20.1_0": "Kathañca, bhikkhave, sappuriso sappurisavāco hoti?",
  "mn110:20.2_0": "Idha,",
  "mn110:20.2_1": "bhikkhave,",
  "mn110:20.2_2": "sappuriso musāvādā paṭivirato hoti,",
  "mn110:20.2_3": "pisuṇāya vācāya paṭivirato hoti,",
  "mn110:20.2_4": "pharusāya vācāya paṭivirato hoti,",
  "mn110:20.2_5": "samphappalāpā paṭivirato hoti.",
  "mn110:20.3_0": "Evaṁ kho, bhikkhave, sappuriso sappurisavāco hoti.",
  "mn110:21.1_0": "Kathañca, bhikkhave, sappuriso sappurisakammanto hoti?",
  "mn110:21.2_0": "Idha,",
  "mn110:21.2_1": "bhikkhave,",
  "mn110:21.2_2": "sappuriso pāṇātipātā paṭivirato hoti,",
  "mn110:21.2_3": "adinnādānā paṭivirato hoti,",
  "mn110:21.2_4": "kāmesumicchācārā paṭivirato hoti.",
  "mn110:21.3_0": "Evaṁ kho, bhikkhave, sappuriso sappurisakammanto hoti.",
  "mn110:22.1_0": "Kathañca, bhikkhave, sappuriso sappurisadiṭṭhi hoti?",
  "mn110:22.2_0": "Idha, bhikkhave, sappuriso evaṁdiṭṭhi hoti:",
  "mn110:22.3_0": "‘atthi dinnaṁ,",
  "mn110:22.3_1": "atthi yiṭṭhaṁ,",
  "mn110:22.3_2": "atthi hutaṁ,",
  "mn110:22.3_3": "atthi sukatadukkaṭānaṁ kammānaṁ phalaṁ vipāko,",
  "mn110:22.3_4": "atthi ayaṁ loko,",
  "mn110:22.3_5": "atthi paro loko,",
  "mn110:22.3_6": "atthi mātā,",
  "mn110:22.3_7": "atthi pitā,",
  "mn110:22.3_8": "atthi sattā opapātikā,",
  "mn110:22.3_9": "atthi loke samaṇabrāhmaṇā sammaggatā sammāpaṭipannā ye imañca lokaṁ parañca lokaṁ sayaṁ abhiññā sacchikatvā pavedentī’ti.",
  "mn110:22.4_0": "Evaṁ kho, bhikkhave, sappuriso sappurisadiṭṭhi hoti.",
  "mn110:23.1_0": "Kathañca, bhikkhave, sappuriso sappurisadānaṁ deti?",
  "mn110:23.2_0": "Idha,",
  "mn110:23.2_1": "bhikkhave,",
  "mn110:23.2_2": "sappuriso sakkaccaṁ dānaṁ deti,",
  "mn110:23.2_3": "sahatthā dānaṁ deti,",
  "mn110:23.2_4": "cittīkatvā dānaṁ deti,",
  "mn110:23.2_5": "anapaviṭṭhaṁ dānaṁ deti,",
  "mn110:23.2_6": "āgamanadiṭṭhiko dānaṁ deti.",
  "mn110:23.3_0": "Evaṁ kho, bhikkhave, sappuriso sappurisadānaṁ deti.",
  "mn110:24.1_0": "So,",
  "mn110:24.1_1": "bhikkhave,",
  "mn110:24.1_2": "sappuriso evaṁ saddhammasamannāgato,",
  "mn110:24.1_3": "evaṁ sappurisabhatti,",
  "mn110:24.1_4": "evaṁ sappurisacintī,",
  "mn110:24.1_5": "evaṁ sappurisamantī,",
  "mn110:24.1_6": "evaṁ sappurisavāco,",
  "mn110:24.1_7": "evaṁ sappurisakammanto,",
  "mn110:24.1_8": "evaṁ sappurisadiṭṭhi;",
  "mn110:24.2_0": "evaṁ sappurisadānaṁ datvā kāyassa bhedā paraṁ maraṇā yā sappurisānaṁ gati tattha upapajjati.",
  "mn110:24.3_0": "Kā ca, bhikkhave, sappurisānaṁ gati?",
  "mn110:24.4_0": "Devamahattatā vā manussamahattatā vā”ti.",
  "mn110:25.1_0": "Idamavoca bhagavā.",
  "mn110:25.2_0": "Attamanā te bhikkhū bhagavato bhāsitaṁ abhinandunti.",
  "mn110:25.3_0": "Cūḷapuṇṇamasuttaṁ niṭṭhitaṁ dasamaṁ.",
  "mn110:25.4_0": "Devadahavaggo niṭṭhito paṭhamo.",
  "mn110:26.0_0": "Tassuddānaṁ",
  "mn110:26.1_0": "Devadahaṁ pañcattayaṁ,",
  "mn110:26.2_0": "Kinti sāma sunakkhattaṁ;",
  "mn110:26.3_0": "Sappāya gaṇa gopaka—",
  "mn110:26.4_0": "Mahāpuṇṇa cūḷapuṇṇañcāti."
}