{
  "mil3.5.5:0.1_0": "Milindapañha",
  "mil3.5.5:0.2_0": "Buddhavagga",
  "mil3.5.5:0.3_0": "5. Asaṅkamanapaṭisandahanapañha",
  "mil3.5.5:1.1_0": "Rājā āha—",
  "mil3.5.5:1.2_0": "“bhante nāgasena,",
  "mil3.5.5:1.2_1": "na ca saṅkamati paṭisandahati cā”ti?",
  "mil3.5.5:1.3_0": "“Āma, mahārāja,",
  "mil3.5.5:1.3_1": "na ca saṅkamati paṭisandahati cā”ti.",
  "mil3.5.5:1.4_0": "“Kathaṁ, bhante nāgasena,",
  "mil3.5.5:1.4_1": "na ca saṅkamati paṭisandahati ca, opammaṁ karohī”ti?",
  "mil3.5.5:1.5_0": "“Yathā, mahārāja,",
  "mil3.5.5:1.5_1": "kocideva puriso padīpato padīpaṁ padīpeyya, kiṁ nu kho so, mahārāja, padīpo padīpamhā saṅkanto”ti?",
  "mil3.5.5:1.6_0": "“Na hi, bhante”ti.",
  "mil3.5.5:1.7_0": "“Evameva kho, mahārāja,",
  "mil3.5.5:1.7_1": "na ca saṅkamati paṭisandahati cā”ti.",
  "mil3.5.5:2.1_0": "“Bhiyyo opammaṁ karohī”ti.",
  "mil3.5.5:2.2_0": "“Abhijānāsi nu tvaṁ, mahārāja,",
  "mil3.5.5:2.2_1": "daharako santo silokācariyassa santike kiñci silokaṁ gahitan”ti?",
  "mil3.5.5:2.3_0": "“Āma, bhante”ti.",
  "mil3.5.5:2.4_0": "“Kiṁ nu kho, mahārāja,",
  "mil3.5.5:2.4_1": "so siloko ācariyamhā saṅkanto”ti?",
  "mil3.5.5:2.5_0": "“Na hi, bhante”ti.",
  "mil3.5.5:2.6_0": "“Evameva kho, mahārāja,",
  "mil3.5.5:2.6_1": "na ca saṅkamati paṭisandahati cā”ti.",
  "mil3.5.5:3.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.5.5:4.1_0": "Asaṅkamanapaṭisandahanapañho pañcamo."
}