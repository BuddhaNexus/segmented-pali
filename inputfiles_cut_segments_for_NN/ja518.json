{
  "ja518:0.1_0": "Jātaka",
  "ja518:0.2_0": "Tiṁsanipāta",
  "ja518:0.3_0": "Kiṁchandavagga",
  "ja518:0.4_0": "8. Paṇḍaranāgarājajātaka",
  "ja518:1.1_0": "“Vikiṇṇavācaṁ aniguyha mantaṁ,",
  "ja518:1.2_0": "Asaññataṁ aparicakkhitāraṁ;",
  "ja518:1.3_0": "Bhayaṁ tamanveti sayaṁ abodhaṁ,",
  "ja518:1.4_0": "Nāgaṁ yathā paṇḍarakaṁ supaṇṇo.",
  "ja518:2.1_0": "Yo guyhamantaṁ parirakkhaṇeyyaṁ,",
  "ja518:2.2_0": "Mohā naro saṁsati hāsamāno;",
  "ja518:2.3_0": "Taṁ bhinnamantaṁ bhayamanveti khippaṁ,",
  "ja518:2.4_0": "Nāgaṁ yathā paṇḍarakaṁ supaṇṇo.",
  "ja518:3.1_0": "Nānumitto garuṁ atthaṁ,",
  "ja518:3.2_0": "guyhaṁ veditumarahati;",
  "ja518:3.3_0": "Sumitto ca asambuddhaṁ,",
  "ja518:3.4_0": "sambuddhaṁ vā anatthavā.",
  "ja518:4.1_0": "Vissāsamāpajjimahaṁ acelaṁ,",
  "ja518:4.2_0": "Samaṇo ayaṁ sammato bhāvitatto;",
  "ja518:4.3_0": "Tassāhamakkhiṁ vivariṁ guyhamatthaṁ,",
  "ja518:4.4_0": "Atītamattho kapaṇaṁ rudāmi.",
  "ja518:5.1_0": "Tassāhaṁ paramaṁ brahme guyhaṁ,",
  "ja518:5.2_0": "Vācañhi maṁ nāsakkhiṁ saṁyametuṁ;",
  "ja518:5.3_0": "Tappakkhato hi bhayamāgataṁ mamaṁ,",
  "ja518:5.4_0": "Atītamattho kapaṇaṁ rudāmi.",
  "ja518:6.1_0": "Yo ve naro suhadaṁ maññamāno,",
  "ja518:6.2_0": "Guyhamatthaṁ saṁsati dukkulīne;",
  "ja518:6.3_0": "Dosā bhayā athavā rāgarattā,",
  "ja518:6.4_0": "Pallatthito bālo asaṁsayaṁ so.",
  "ja518:7.1_0": "Tirokkhavāco asataṁ paviṭṭho,",
  "ja518:7.2_0": "Yo saṅgatīsu mudīreti vākyaṁ;",
  "ja518:7.3_0": "Āsīviso dummukhotyāhu taṁ naraṁ,",
  "ja518:7.4_0": "Ārā ārā saṁyame tādisamhā.",
  "ja518:8.1_0": "Annaṁ pānaṁ kāsikacandanañca,",
  "ja518:8.2_0": "Manāpitthiyo mālamucchādanañca;",
  "ja518:8.3_0": "Ohāya gacchāmase sabbakāme,",
  "ja518:8.4_0": "Supaṇṇa pāṇūpagatāva tyamhā”.",
  "ja518:9.1_0": "“Ko nīdha tiṇṇaṁ garahaṁ upeti,",
  "ja518:9.2_0": "Asmiṁdhaloke pāṇabhū nāgarāja;",
  "ja518:9.3_0": "Samaṇo supaṇṇo athavā tvameva,",
  "ja518:9.4_0": "Kiṁ kāraṇā paṇḍarakaggahīto”.",
  "ja518:10.1_0": "“Samaṇoti me sammatatto ahosi,",
  "ja518:10.2_0": "Piyo ca me manasā bhāvitatto;",
  "ja518:10.3_0": "Tassāhamakkhiṁ vivariṁ guyhamatthaṁ,",
  "ja518:10.4_0": "Atītamattho kapaṇaṁ rudāmi”.",
  "ja518:11.1_0": "“Na catthi satto amaro pathabyā,",
  "ja518:11.2_0": "Paññāvidhā natthi na ninditabbā;",
  "ja518:11.3_0": "Saccena dhammena dhitiyā damena,",
  "ja518:11.4_0": "Alabbhamabyāharatī naro idha.",
  "ja518:12.1_0": "Mātāpitā paramā bandhavānaṁ,",
  "ja518:12.2_0": "Nāssa tatiyo anukampakatthi;",
  "ja518:12.3_0": "Tesampi guyhaṁ paramaṁ na saṁse,",
  "ja518:12.4_0": "Mantassa bhedaṁ parisaṅkamāno.",
  "ja518:13.1_0": "Mātāpitā bhaginī bhātaro ca,",
  "ja518:13.2_0": "Sahāyā vā yassa honti sapakkhā;",
  "ja518:13.3_0": "Tesampi guyhaṁ paramaṁ na saṁse,",
  "ja518:13.4_0": "Mantassa bhedaṁ parisaṅkamāno.",
  "ja518:14.1_0": "Bhariyā ce purisaṁ vajjā,",
  "ja518:14.2_0": "Komārī piyabhāṇinī;",
  "ja518:14.3_0": "Puttarūpayasūpetā,",
  "ja518:14.4_0": "Ñātisaṅghapurakkhatā;",
  "ja518:14.5_0": "Tassāpi guyhaṁ paramaṁ na saṁse,",
  "ja518:14.6_0": "Mantassa bhedaṁ parisaṅkamāno.",
  "ja518:15.1_0": "Na guyhamatthaṁ vivareyya,",
  "ja518:15.2_0": "Rakkheyya naṁ yathā nidhiṁ;",
  "ja518:15.3_0": "Na hi pātukato sādhu,",
  "ja518:15.4_0": "Guyho attho pajānatā.",
  "ja518:16.1_0": "Thiyā guyhaṁ na saṁseyya,",
  "ja518:16.2_0": "amittassa ca paṇḍito;",
  "ja518:16.3_0": "Yo cāmisena saṁhīro,",
  "ja518:16.4_0": "hadayattheno ca yo naro.",
  "ja518:17.1_0": "Guyhamatthaṁ asambuddhaṁ,",
  "ja518:17.2_0": "sambodhayati yo naro;",
  "ja518:17.3_0": "Mantabhedabhayā tassa,",
  "ja518:17.4_0": "dāsabhūto titikkhati.",
  "ja518:18.1_0": "Yāvanto purisassatthaṁ,",
  "ja518:18.2_0": "guyhaṁ jānanti mantinaṁ;",
  "ja518:18.3_0": "Tāvanto tassa ubbegā,",
  "ja518:18.4_0": "tasmā guyhaṁ na vissaje.",
  "ja518:19.1_0": "Vivicca bhāseyya divā rahassaṁ,",
  "ja518:19.2_0": "Rattiṁ giraṁ nātivelaṁ pamuñce;",
  "ja518:19.3_0": "Upassutikā hi suṇanti mantaṁ,",
  "ja518:19.4_0": "Tasmā manto khippamupeti bhedaṁ.",
  "ja518:20.1_0": "Yathāpi assa nagaraṁ mahantaṁ,",
  "ja518:20.2_0": "Advārakaṁ āyasaṁ bhaddasālaṁ;",
  "ja518:20.3_0": "Samantakhātāparikhāupetaṁ,",
  "ja518:20.4_0": "Evampi me te idha guyhamantā.",
  "ja518:21.1_0": "Ye guyhamantā avikiṇṇavācā,",
  "ja518:21.2_0": "Daḷhā sadatthesu narā dujivha;",
  "ja518:21.3_0": "Ārā amittā byavajanti tehi,",
  "ja518:21.4_0": "Āsīvisā vā riva sattusaṅghā”.",
  "ja518:22.1_0": "“Hitvā gharaṁ pabbajito acelo,",
  "ja518:22.2_0": "Naggo muṇḍo carati ghāsahetu;",
  "ja518:22.3_0": "Tamhi nu kho vivariṁ guyhamatthaṁ,",
  "ja518:22.4_0": "Atthā ca dhammā ca apaggatamhā.",
  "ja518:23.1_0": "Kathaṅkaro hoti supaṇṇarāja,",
  "ja518:23.2_0": "Kiṁsīlo kena vatena vattaṁ;",
  "ja518:23.3_0": "Samaṇo caraṁ hitvā mamāyitāni,",
  "ja518:23.4_0": "Kathaṅkaro saggamupeti ṭhānaṁ”.",
  "ja518:24.1_0": "“Hiriyā titikkhāya damenupeto,",
  "ja518:24.2_0": "Akkodhano pesuṇiyaṁ pahāya;",
  "ja518:24.3_0": "Samaṇo caraṁ hitvā mamāyitāni,",
  "ja518:24.4_0": "Evaṅkaro saggamupeti ṭhānaṁ”.",
  "ja518:25.1_0": "“Mātāva puttaṁ taruṇaṁ tanujjaṁ,",
  "ja518:25.2_0": "Samphassatā sabbagattaṁ phareti;",
  "ja518:25.3_0": "Evampi me tvaṁ pāturahu dijinda,",
  "ja518:25.4_0": "Mātāva puttaṁ anukampamāno”.",
  "ja518:26.1_0": "“Handajja tvaṁ muñca vadhā dujivha,",
  "ja518:26.2_0": "Tayo hi puttā na hi añño atthi;",
  "ja518:26.3_0": "Antevāsī dinnako atrajo ca,",
  "ja518:26.4_0": "Rajjassu puttaññataro me ahosi”.",
  "ja518:27.1_0": "Icceva vākyaṁ visajjī supaṇṇo,",
  "ja518:27.2_0": "Bhumyaṁ patiṭṭhāya dijo dujivhaṁ;",
  "ja518:27.3_0": "“Muttajja tvaṁ sabbabhayātivatto,",
  "ja518:27.4_0": "Thalūdake hohi mayābhigutto.",
  "ja518:28.1_0": "Ātaṅkinaṁ yathā kusalo bhisakko,",
  "ja518:28.2_0": "Pipāsitānaṁ rahadova sīto;",
  "ja518:28.3_0": "Vesmaṁ yathā himasītaṭṭitānaṁ,",
  "ja518:28.4_0": "Evampi te saraṇamahaṁ bhavāmi”.",
  "ja518:29.1_0": "“Sandhiṁ katvā amittena,",
  "ja518:29.2_0": "aṇḍajena jalābuja;",
  "ja518:29.3_0": "Vivariya dāṭhaṁ sesi,",
  "ja518:29.4_0": "kuto taṁ bhayamāgataṁ”.",
  "ja518:30.1_0": "“Saṅketheva amittasmiṁ,",
  "ja518:30.2_0": "mittasmimpi na vissase;",
  "ja518:30.3_0": "Abhayā bhayamuppannaṁ,",
  "ja518:30.4_0": "api mūlāni kantati.",
  "ja518:31.1_0": "Kathaṁ nu vissase tyamhi,",
  "ja518:31.2_0": "yenāsi kalaho kato;",
  "ja518:31.3_0": "Niccayattena ṭhātabbaṁ,",
  "ja518:31.4_0": "so disabbhi na rajjati.",
  "ja518:32.1_0": "Vissāsaye na ca taṁ vissayeyya,",
  "ja518:32.2_0": "Asaṅkito saṅkito ca bhaveyya;",
  "ja518:32.3_0": "Tathā tathā viññū parakkameyya,",
  "ja518:32.4_0": "Yathā yathā bhāvaṁ paro na jaññā”.",
  "ja518:33.1_0": "Te devavaṇṇā sukhumālarūpā,",
  "ja518:33.2_0": "Ubho samā sujayā puññakhandhā;",
  "ja518:33.3_0": "Upāgamuṁ karampiyaṁ acelaṁ,",
  "ja518:33.4_0": "Missībhūtā assavāhāva nāgā.",
  "ja518:34.1_0": "Tato have paṇḍarako acelaṁ,",
  "ja518:34.2_0": "Sayamevupāgamma idaṁ avoca;",
  "ja518:34.3_0": "“Muttajjahaṁ sabbabhayātivatto,",
  "ja518:34.4_0": "Na hi nūna tuyhaṁ manaso piyamhā”.",
  "ja518:35.1_0": "“Piyo hi me āsi supaṇṇarājā,",
  "ja518:35.2_0": "Asaṁsayaṁ paṇḍarakena saccaṁ;",
  "ja518:35.3_0": "So rāgarattova akāsimetaṁ,",
  "ja518:35.4_0": "Pāpakammaṁ sampajāno na mohā”.",
  "ja518:36.1_0": "“Na me piyaṁ appiyaṁ vāpi hoti,",
  "ja518:36.2_0": "Sampassato lokamimaṁ parañca;",
  "ja518:36.3_0": "Susaññatānañhi viyañjanena,",
  "ja518:36.4_0": "Asaññato lokamimaṁ carāsi.",
  "ja518:37.1_0": "Ariyāvakāsosi anariyovāsi,",
  "ja518:37.2_0": "Asaññato saññatasannikāso;",
  "ja518:37.3_0": "Kaṇhābhijātikosi anariyarūpo,",
  "ja518:37.4_0": "Pāpaṁ bahuṁ duccaritaṁ acāri.",
  "ja518:38.1_0": "Aduṭṭhassa tuvaṁ dubbhi,",
  "ja518:38.2_0": "dubbhī ca pisuṇo casi;",
  "ja518:38.3_0": "Etena saccavajjena,",
  "ja518:38.4_0": "muddhā te phalatu sattadhā”.",
  "ja518:39.1_0": "“Tasmā hi mittānaṁ na dubbhitabbaṁ,",
  "ja518:39.2_0": "Mittadubbhā pāpiyo natthi añño;",
  "ja518:39.3_0": "Āsittasatto nihato pathabyā,",
  "ja518:39.4_0": "Indassa vākyena hi saṁvaro hato”ti.",
  "ja518:40.1_0": "Paṇḍaranāgarājajātakaṁ aṭṭhamaṁ."
}