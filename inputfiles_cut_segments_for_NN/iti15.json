{
  "iti15:0.1_0": "Itivuttaka 15",
  "iti15:0.2_0": "Ekakanipāta",
  "iti15:0.3_0": "Dutiyavagga",
  "iti15:0.4_0": "Taṇhāsaṁyojanasutta",
  "iti15:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti15:2.1_0": "“Nāhaṁ, bhikkhave,",
  "iti15:2.1_1": "aññaṁ ekasaṁyojanampi samanupassāmi yena saṁyojanena saṁyuttā sattā dīgharattaṁ sandhāvanti saṁsaranti yathayidaṁ, bhikkhave, taṇhāsaṁyojanaṁ.",
  "iti15:2.2_0": "Taṇhāsaṁyojanena hi, bhikkhave,",
  "iti15:2.2_1": "saṁyuttā sattā dīgharattaṁ sandhāvanti saṁsarantī”ti.",
  "iti15:2.3_0": "Etamatthaṁ bhagavā avoca.",
  "iti15:2.4_0": "Tatthetaṁ iti vuccati:",
  "iti15:3.1_0": "“Taṇhādutiyo puriso,",
  "iti15:3.2_0": "dīghamaddhāna saṁsaraṁ;",
  "iti15:3.3_0": "Itthabhāvaññathābhāvaṁ,",
  "iti15:3.4_0": "saṁsāraṁ nātivattati.",
  "iti15:4.1_0": "Etamādīnavaṁ ñatvā,",
  "iti15:4.2_0": "Taṇhaṁ dukkhassa sambhavaṁ;",
  "iti15:4.3_0": "Vītataṇho anādāno,",
  "iti15:4.4_0": "Sato bhikkhu paribbaje”ti.",
  "iti15:5.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti15:5.2_0": "Pañcamaṁ."
}