{
  "ne26:0.1_0": "Netti",
  "ne26:0.2_0": "Paṭiniddesavāra",
  "ne26:0.3_0": "Sampāta 7",
  "ne26:0.4_0": "Āvaṭṭahārasampāta",
  "ne26:1.1_0": "Tattha katamo āvaṭṭo hārasampāto?",
  "ne26:2.1_0": "“Tasmā rakkhitacittassa, sammāsaṅkappagocaro”ti gāthā.",
  "ne26:2.2_0": "“Tasmā rakkhitacittassa,",
  "ne26:2.2_1": "sammāsaṅkappagocaro”ti samatho.",
  "ne26:2.3_0": "“Sammādiṭṭhipurekkhāro”ti vipassanā.",
  "ne26:2.4_0": "“Ñatvāna udayabbayan”ti dukkhapariññā.",
  "ne26:2.5_0": "“Thinamiddhābhibhū bhikkhū”ti samudayapahānaṁ.",
  "ne26:2.6_0": "“Sabbā duggatiyo jahe”ti nirodho.",
  "ne26:2.7_0": "Imāni cattāri saccāni.",
  "ne26:2.8_0": "Niyutto āvaṭṭo hārasampāto."
}