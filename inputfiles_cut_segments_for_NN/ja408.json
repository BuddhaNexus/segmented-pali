{
  "ja408:0.1_0": "Jātaka",
  "ja408:0.2_0": "Sattakanipāta",
  "ja408:0.3_0": "Gandhāravagga",
  "ja408:0.4_0": "3. Kumbhakārajātaka",
  "ja408:1.1_0": "“Ambāhamaddaṁ vanamantarasmiṁ,",
  "ja408:1.2_0": "Nīlobhāsaṁ phalitaṁ saṁvirūḷhaṁ;",
  "ja408:1.3_0": "Tamaddasaṁ phalahetu vibhaggaṁ,",
  "ja408:1.4_0": "Taṁ disvā bhikkhācariyaṁ carāmi.",
  "ja408:2.1_0": "Selaṁ sumaṭṭhaṁ naravīraniṭṭhitaṁ,",
  "ja408:2.2_0": "Nārī yugaṁ dhārayi appasaddaṁ;",
  "ja408:2.3_0": "Dutiyañca āgamma ahosi saddo,",
  "ja408:2.4_0": "Taṁ disvā bhikkhācariyaṁ carāmi.",
  "ja408:3.1_0": "Dijā dijaṁ kuṇapamāharantaṁ,",
  "ja408:3.2_0": "Ekaṁ samānaṁ bahukā samecca;",
  "ja408:3.3_0": "Āhārahetū paripātayiṁsu,",
  "ja408:3.4_0": "Taṁ disvā bhikkhācariyaṁ carāmi.",
  "ja408:4.1_0": "Usabhāhamaddaṁ yūthassa majjhe,",
  "ja408:4.2_0": "Calakkakuṁ vaṇṇabalūpapannaṁ;",
  "ja408:4.3_0": "Tamaddasaṁ kāmahetu vitunnaṁ,",
  "ja408:4.4_0": "Taṁ disvā bhikkhācariyaṁ carāmi”.",
  "ja408:5.1_0": "“Karaṇḍako kaliṅgānaṁ,",
  "ja408:5.2_0": "gandhārānañca naggaji;",
  "ja408:5.3_0": "Nimirājā videhānaṁ,",
  "ja408:5.4_0": "pañcālānañca dummukho;",
  "ja408:5.5_0": "Ete raṭṭhāni hitvāna,",
  "ja408:5.6_0": "pabbajiṁsu akiñcanā.",
  "ja408:6.1_0": "Sabbepime devasamā samāgatā,",
  "ja408:6.2_0": "Aggī yathā pajjalito tathevime;",
  "ja408:6.3_0": "Ahampi eko carissāmi bhaggavi,",
  "ja408:6.4_0": "Hitvāna kāmāni yathodhikāni”.",
  "ja408:7.1_0": "“Ayameva kālo na hi añño atthi,",
  "ja408:7.2_0": "Anusāsitā me na bhaveyya pacchā;",
  "ja408:7.3_0": "Ahampi ekā carissāmi bhaggava,",
  "ja408:7.4_0": "Sakuṇīva muttā purisassa hatthā”.",
  "ja408:8.1_0": "“Āmaṁ pakkañca jānanti,",
  "ja408:8.2_0": "atho loṇaṁ aloṇakaṁ;",
  "ja408:8.3_0": "Tamahaṁ disvāna pabbajiṁ,",
  "ja408:8.4_0": "careva tvaṁ carāmahan”ti.",
  "ja408:9.1_0": "Kumbhakārajātakaṁ tatiyaṁ."
}