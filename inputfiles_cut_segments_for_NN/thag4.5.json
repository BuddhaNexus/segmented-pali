{
  "thag4.5:0.1_0": "Theragāthā",
  "thag4.5:0.2_0": "Catukkanipāta",
  "thag4.5:0.3_0": "Paṭhamavagga",
  "thag4.5:0.4_0": "4.5. Jambukattheragāthā",
  "thag4.5:1.1_0": "“Pañcapaññāsavassāni,",
  "thag4.5:1.2_0": "rajojallamadhārayiṁ;",
  "thag4.5:1.3_0": "Bhuñjanto māsikaṁ bhattaṁ,",
  "thag4.5:1.4_0": "kesamassuṁ alocayiṁ.",
  "thag4.5:2.1_0": "Ekapādena aṭṭhāsiṁ,",
  "thag4.5:2.2_0": "āsanaṁ parivajjayiṁ;",
  "thag4.5:2.3_0": "Sukkhagūthāni ca khādiṁ,",
  "thag4.5:2.4_0": "uddesañca na sādiyiṁ.",
  "thag4.5:3.1_0": "Etādisaṁ karitvāna,",
  "thag4.5:3.2_0": "bahuṁ duggatigāminaṁ;",
  "thag4.5:3.3_0": "Vuyhamāno mahoghena,",
  "thag4.5:3.4_0": "buddhaṁ saraṇamāgamaṁ.",
  "thag4.5:4.1_0": "Saraṇagamanaṁ passa,",
  "thag4.5:4.2_0": "passa dhammasudhammataṁ;",
  "thag4.5:4.3_0": "Tisso vijjā anuppattā,",
  "thag4.5:4.4_0": "kataṁ buddhassa sāsanan”ti.",
  "thag4.5:4.5_0": "… Jambuko thero …."
}