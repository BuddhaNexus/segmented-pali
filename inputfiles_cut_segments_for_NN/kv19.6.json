{
  "kv19.6:0.1_0": "Kathāvatthu",
  "kv19.6:0.2_0": "Mahāpaṇṇāsaka",
  "kv19.6:0.3_0": "Ekūnavīsatimavagga",
  "kv19.6:0.4_0": "Kusalakathā",
  "kv19.6:1.1_0": "Nibbānadhātu kusalāti?",
  "kv19.6:1.2_0": "Āmantā.",
  "kv19.6:1.3_0": "Sārammaṇā, atthi tāya āvaṭṭanā …pe… paṇidhīti?",
  "kv19.6:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv19.6:1.5_0": "nanu anārammaṇā,",
  "kv19.6:1.5_1": "natthi tāya āvaṭṭanā …pe… paṇidhīti?",
  "kv19.6:1.6_0": "Āmantā.",
  "kv19.6:1.7_0": "Hañci anārammaṇā,",
  "kv19.6:1.7_1": "natthi tāya āvaṭṭanā …pe… paṇidhi, no ca vata re vattabbe—",
  "kv19.6:1.8_0": "“nibbānadhātu kusalā”ti.",
  "kv19.6:2.1_0": "Alobho kusalo sārammaṇo,",
  "kv19.6:2.1_1": "atthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv19.6:2.2_0": "Āmantā.",
  "kv19.6:2.3_0": "Nibbānadhātu kusalā sārammaṇā,",
  "kv19.6:2.3_1": "atthi tāya āvaṭṭanā …pe… paṇidhīti?",
  "kv19.6:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv19.6:2.5_0": "adoso kusalo …pe…",
  "kv19.6:2.6_0": "amoho kusalo …pe…",
  "kv19.6:2.7_0": "saddhā …",
  "kv19.6:2.8_0": "vīriyaṁ …",
  "kv19.6:2.9_0": "sati …",
  "kv19.6:2.10_0": "samādhi …pe…",
  "kv19.6:2.11_0": "paññā kusalā sārammaṇā,",
  "kv19.6:2.11_1": "atthi tāya āvaṭṭanā …pe… paṇidhīti?",
  "kv19.6:2.12_0": "Āmantā.",
  "kv19.6:2.13_0": "Nibbānadhātu kusalā sārammaṇā,",
  "kv19.6:2.13_1": "atthi tāya āvaṭṭanā …pe… paṇidhīti?",
  "kv19.6:2.14_0": "Na hevaṁ vattabbe …pe….",
  "kv19.6:3.1_0": "Nibbānadhātu kusalā anārammaṇā,",
  "kv19.6:3.1_1": "natthi tāya āvaṭṭanā …pe… paṇidhīti?",
  "kv19.6:3.2_0": "Āmantā.",
  "kv19.6:3.3_0": "Alobho kusalo anārammaṇo,",
  "kv19.6:3.3_1": "natthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv19.6:3.4_0": "Na hevaṁ vattabbe …pe…",
  "kv19.6:3.5_0": "nibbānadhātu kusalā anārammaṇā,",
  "kv19.6:3.5_1": "natthi tāya āvaṭṭanā …pe… paṇidhīti?",
  "kv19.6:3.6_0": "Āmantā.",
  "kv19.6:3.7_0": "Adoso kusalo …pe…",
  "kv19.6:3.8_0": "amoho kusalo …pe…",
  "kv19.6:3.9_0": "saddhā …",
  "kv19.6:3.10_0": "vīriyaṁ …",
  "kv19.6:3.11_0": "sati …",
  "kv19.6:3.12_0": "samādhi …pe…",
  "kv19.6:3.13_0": "paññā kusalā anārammaṇā,",
  "kv19.6:3.13_1": "natthi tāya āvaṭṭanā …pe… paṇidhīti?",
  "kv19.6:3.14_0": "Na hevaṁ vattabbe …pe….",
  "kv19.6:4.1_0": "Na vattabbaṁ—",
  "kv19.6:4.2_0": "“nibbānadhātu kusalā”ti?",
  "kv19.6:4.3_0": "Āmantā.",
  "kv19.6:4.4_0": "Nanu nibbānadhātu anavajjāti?",
  "kv19.6:4.5_0": "Āmantā.",
  "kv19.6:4.6_0": "Hañci nibbānadhātu anavajjā, tena vata re vattabbe—",
  "kv19.6:4.7_0": "“nibbānadhātu kusalā”ti.",
  "kv19.6:4.8_0": "Kusalakathā niṭṭhitā."
}