{
  "ja130:0.1_0": "Jātaka",
  "ja130:0.2_0": "Ekakanipāta",
  "ja130:0.3_0": "Kusanāḷivagga",
  "ja130:0.4_0": "10. Kosiyajātaka",
  "ja130:1.1_0": "“Yathā vācā ca bhuñjassu,",
  "ja130:1.2_0": "yathā bhuttañca byāhara;",
  "ja130:1.3_0": "Ubhayaṁ te na sameti,",
  "ja130:1.4_0": "vācā bhuttañca kosiye”ti.",
  "ja130:2.1_0": "Kosiyajātakaṁ dasamaṁ.",
  "ja130:2.2_0": "Kusanāḷivaggo terasamo.",
  "ja130:3.1_0": "Tassuddānaṁ",
  "ja130:4.1_0": "Kusanāḷisirivhayano ca yasaṁ,",
  "ja130:4.2_0": "Dadhi mamba kaṭāhakapañcamako;",
  "ja130:4.3_0": "Atha pāpaka khīra biḷāravataṁ,",
  "ja130:4.4_0": "Sikhi kosiyasavhayanena dasāti."
}