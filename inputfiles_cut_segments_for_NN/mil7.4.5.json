{
  "mil7.4.5:0.1_0": "Milindapañha",
  "mil7.4.5:0.2_0": "Opammakathāpañha",
  "mil7.4.5:0.3_0": "Pathavīvagga",
  "mil7.4.5:0.4_0": "5. Pabbataṅgapañha",
  "mil7.4.5:1.1_0": "“Bhante nāgasena,",
  "mil7.4.5:1.1_1": "‘pabbatassa pañca aṅgāni gahetabbānī’ti yaṁ vadesi,",
  "mil7.4.5:1.1_2": "katamāni tāni pañca aṅgāni gahetabbānī”ti?",
  "mil7.4.5:1.2_0": "“Yathā, mahārāja,",
  "mil7.4.5:1.2_1": "pabbato acalo akampito asampavedhī;",
  "mil7.4.5:1.3_0": "evameva kho, mahārāja,",
  "mil7.4.5:1.3_1": "yoginā yogāvacarena sammānane vimānane sakkāre asakkāre garukāre agarukāre yase ayase nindāya pasaṁsāya sukhe dukkhe iṭṭhāniṭṭhesu sabbattha rūpasaddagandharasaphoṭṭhabbadhammesu rajanīyesu na rajjitabbaṁ, dussanīyesu na dussitabbaṁ, muyhanīyesu na muyhitabbaṁ,",
  "mil7.4.5:1.3_2": "na kampitabbaṁ na calitabbaṁ,",
  "mil7.4.5:1.3_3": "pabbatena viya acalena bhavitabbaṁ.",
  "mil7.4.5:1.4_0": "Idaṁ, mahārāja,",
  "mil7.4.5:1.4_1": "pabbatassa paṭhamaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.4.5:1.5_0": "Bhāsitampetaṁ, mahārāja, bhagavatā devātidevena—",
  "mil7.4.5:2.1_0": "‘Selo yathā ekaghano,",
  "mil7.4.5:2.2_0": "vātena na samīrati;",
  "mil7.4.5:2.3_0": "Evaṁ nindāpasaṁsāsu,",
  "mil7.4.5:2.4_0": "na samiñjanti paṇḍitā’ti.",
  "mil7.4.5:3.1_0": "Puna caparaṁ, mahārāja,",
  "mil7.4.5:3.1_1": "pabbato thaddho na kenaci saṁsaṭṭho;",
  "mil7.4.5:3.2_0": "evameva kho, mahārāja,",
  "mil7.4.5:3.2_1": "yoginā yogāvacarena thaddhena asaṁsaṭṭhena bhavitabbaṁ, na kenaci saṁsaggo karaṇīyo.",
  "mil7.4.5:3.3_0": "Idaṁ, mahārāja,",
  "mil7.4.5:3.3_1": "pabbatassa dutiyaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.4.5:3.4_0": "Bhāsitampetaṁ, mahārāja, bhagavatā devātidevena—",
  "mil7.4.5:4.1_0": "‘Asaṁsaṭṭhaṁ gahaṭṭhehi,",
  "mil7.4.5:4.2_0": "anāgārehi cūbhayaṁ;",
  "mil7.4.5:4.3_0": "Anokasārimappicchaṁ,",
  "mil7.4.5:4.4_0": "tamahaṁ brūmi brāhmaṇan’ti.",
  "mil7.4.5:5.1_0": "Puna caparaṁ, mahārāja, pabbate bījaṁ na virūhati;",
  "mil7.4.5:5.2_0": "evameva kho, mahārāja,",
  "mil7.4.5:5.2_1": "yoginā yogāvacarena sakamānase kilesā na virūhāpetabbā.",
  "mil7.4.5:5.3_0": "Idaṁ, mahārāja,",
  "mil7.4.5:5.3_1": "pabbatassa tatiyaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.4.5:5.4_0": "Bhāsitampetaṁ, mahārāja, therena subhūtinā—",
  "mil7.4.5:6.1_0": "‘Rāgūpasaṁhitaṁ cittaṁ,",
  "mil7.4.5:6.2_0": "yadā uppajjate mama;",
  "mil7.4.5:6.3_0": "Sayaṁva paccavekkhāmi,",
  "mil7.4.5:6.4_0": "ekaggo taṁ damemahaṁ.",
  "mil7.4.5:7.1_0": "Rajjase rajanīye ca,",
  "mil7.4.5:7.2_0": "dussanīye ca dussase;",
  "mil7.4.5:7.3_0": "Muyhase mohanīye ca,",
  "mil7.4.5:7.4_0": "nikkhamassu vanā tuvaṁ.",
  "mil7.4.5:8.1_0": "Visuddhānaṁ ayaṁ vāso,",
  "mil7.4.5:8.2_0": "nimmalānaṁ tapassinaṁ;",
  "mil7.4.5:8.3_0": "Mā kho visuddhaṁ dūsesi,",
  "mil7.4.5:8.4_0": "nikkhamassu vanā tuvan’ti.",
  "mil7.4.5:9.1_0": "Puna caparaṁ, mahārāja, pabbato accuggato;",
  "mil7.4.5:9.2_0": "evameva kho, mahārāja,",
  "mil7.4.5:9.2_1": "yoginā yogāvacarena ñāṇaccuggatena bhavitabbaṁ.",
  "mil7.4.5:9.3_0": "Idaṁ, mahārāja,",
  "mil7.4.5:9.3_1": "pabbatassa catutthaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.4.5:9.4_0": "Bhāsitampetaṁ, mahārāja, bhagavatā devātidevena—",
  "mil7.4.5:10.1_0": "‘Pamādaṁ appamādena,",
  "mil7.4.5:10.2_0": "yadā nudati paṇḍito;",
  "mil7.4.5:10.3_0": "Paññāpāsādamāruyha,",
  "mil7.4.5:10.4_0": "asoko sokiniṁ pajaṁ;",
  "mil7.4.5:10.5_0": "Pabbataṭṭhova bhūmaṭṭhe,",
  "mil7.4.5:10.6_0": "dhīro bāle avekkhatī’ti.",
  "mil7.4.5:11.1_0": "Puna caparaṁ, mahārāja, pabbato anunnato anonato;",
  "mil7.4.5:11.2_0": "evameva kho, mahārāja,",
  "mil7.4.5:11.2_1": "yoginā yogāvacarena unnatāvanati na karaṇīyā.",
  "mil7.4.5:11.3_0": "Idaṁ, mahārāja,",
  "mil7.4.5:11.3_1": "pabbatassa pañcamaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.4.5:11.4_0": "Bhāsitampetaṁ, mahārāja,",
  "mil7.4.5:11.4_1": "upāsikāya cūḷasubhaddāya sakasamaṇe parikittayamānāya—",
  "mil7.4.5:12.1_0": "‘Lābhena unnato loko,",
  "mil7.4.5:12.2_0": "alābhena ca onato;",
  "mil7.4.5:12.3_0": "Lābhālābhena ekaṭṭhā,",
  "mil7.4.5:12.4_0": "tādisā samaṇā mamā’”ti.",
  "mil7.4.5:13.1_0": "Pabbataṅgapañho pañcamo."
}