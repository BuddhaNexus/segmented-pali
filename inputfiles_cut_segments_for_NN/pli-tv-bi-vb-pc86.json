{
  "pli-tv-bi-vb-pc86:0.1_0": "Theravāda Vinaya",
  "pli-tv-bi-vb-pc86:0.2_0": "Bhikkhunivibhaṅga",
  "pli-tv-bi-vb-pc86:0.3_0": "Pācittiyakaṇḍa",
  "pli-tv-bi-vb-pc86:0.4_0": "Chattupāhanavagga",
  "pli-tv-bi-vb-pc86:0.5_0": "86. Saṅghāṇisikkhāpada",
  "pli-tv-bi-vb-pc86:1.1_0": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bi-vb-pc86:1.2_0": "Tena kho pana samayena aññatarā bhikkhunī aññatarissā itthiyā kulūpikā hoti.",
  "pli-tv-bi-vb-pc86:1.3_0": "Atha kho sā itthī taṁ bhikkhuniṁ etadavoca—",
  "pli-tv-bi-vb-pc86:1.4_0": "“handāyye,",
  "pli-tv-bi-vb-pc86:1.4_1": "imaṁ saṅghāṇiṁ amukāya nāma itthiyā dehī”ti.",
  "pli-tv-bi-vb-pc86:1.5_0": "Atha kho sā bhikkhunī—",
  "pli-tv-bi-vb-pc86:1.6_0": "“sacāhaṁ pattena ādāya gacchāmi vissaro me bhavissatī”ti paṭimuñcitvā agamāsi.",
  "pli-tv-bi-vb-pc86:1.7_0": "Tassā rathikāya suttake chinne vippakiriyiṁsu.",
  "pli-tv-bi-vb-pc86:1.8_0": "Manussā ujjhāyanti khiyyanti vipācenti—",
  "pli-tv-bi-vb-pc86:1.9_0": "“kathañhi nāma bhikkhuniyo saṅghāṇiṁ dhāressanti,",
  "pli-tv-bi-vb-pc86:1.9_1": "seyyathāpi gihiniyo kāmabhoginiyo”ti.",
  "pli-tv-bi-vb-pc86:1.10_0": "Assosuṁ kho bhikkhuniyo tesaṁ manussānaṁ ujjhāyantānaṁ khiyyantānaṁ vipācentānaṁ.",
  "pli-tv-bi-vb-pc86:1.11_0": "Yā tā bhikkhuniyo appicchā …pe… tā ujjhāyanti khiyyanti vipācenti—",
  "pli-tv-bi-vb-pc86:1.12_0": "“kathañhi nāma bhikkhunī saṅghāṇiṁ dhāressatī”ti …pe…",
  "pli-tv-bi-vb-pc86:1.13_0": "“saccaṁ kira, bhikkhave,",
  "pli-tv-bi-vb-pc86:1.13_1": "bhikkhunī saṅghāṇiṁ dhāretī”ti?",
  "pli-tv-bi-vb-pc86:1.14_0": "“Saccaṁ, bhagavā”ti.",
  "pli-tv-bi-vb-pc86:1.15_0": "Vigarahi buddho bhagavā …pe…",
  "pli-tv-bi-vb-pc86:1.16_0": "kathañhi nāma, bhikkhave,",
  "pli-tv-bi-vb-pc86:1.16_1": "bhikkhunī saṅghāṇiṁ dhāressati.",
  "pli-tv-bi-vb-pc86:1.17_0": "Netaṁ, bhikkhave, appasannānaṁ vā pasādāya …pe…",
  "pli-tv-bi-vb-pc86:1.18_0": "evañca pana, bhikkhave,",
  "pli-tv-bi-vb-pc86:1.18_1": "bhikkhuniyo imaṁ sikkhāpadaṁ uddisantu—",
  "pli-tv-bi-vb-pc86:1.19.1_0": "“Yā pana bhikkhunī saṅghāṇiṁ dhāreyya, pācittiyan”ti.",
  "pli-tv-bi-vb-pc86:2.1.1_0": "Yā panāti",
  "pli-tv-bi-vb-pc86:2.1.2_0": "yā yādisā …pe…",
  "pli-tv-bi-vb-pc86:2.1.3_0": "bhikkhunīti",
  "pli-tv-bi-vb-pc86:2.1.4_0": "…pe… ayaṁ imasmiṁ atthe adhippetā bhikkhunīti.",
  "pli-tv-bi-vb-pc86:2.1.5_0": "Saṅghāṇi nāma",
  "pli-tv-bi-vb-pc86:2.1.6_0": "yā kāci kaṭūpagā.",
  "pli-tv-bi-vb-pc86:2.1.7_0": "Dhāreyyāti",
  "pli-tv-bi-vb-pc86:2.1.8_0": "sakimpi dhāreti, āpatti pācittiyassa.",
  "pli-tv-bi-vb-pc86:2.2.1_0": "Anāpatti—",
  "pli-tv-bi-vb-pc86:2.2.2_0": "ābādhappaccayā,",
  "pli-tv-bi-vb-pc86:2.2.3_0": "kaṭisuttakaṁ dhāreti,",
  "pli-tv-bi-vb-pc86:2.2.4_0": "ummattikāya,",
  "pli-tv-bi-vb-pc86:2.2.5_0": "ādikammikāyāti.",
  "pli-tv-bi-vb-pc86:2.2.6_0": "Tatiyasikkhāpadaṁ niṭṭhitaṁ."
}