{
  "kv12.3:0.1_0": "Kathāvatthu",
  "kv12.3:0.2_0": "Mahāpaṇṇāsaka",
  "kv12.3:0.3_0": "Dvādasamavagga",
  "kv12.3:0.4_0": "Saddovipākotikathā",
  "kv12.3:1.1_0": "Saddo vipākoti?",
  "kv12.3:1.2_0": "Āmantā.",
  "kv12.3:1.3_0": "Sukhavedaniyo dukkhavedaniyo adukkhamasukhavedaniyo, sukhāya vedanāya sampayutto,",
  "kv12.3:1.3_1": "dukkhāya vedanāya sampayutto,",
  "kv12.3:1.3_2": "adukkhamasukhāya vedanāya sampayutto, phassena sampayutto, vedanāya sampayutto, saññāya sampayutto, cetanāya sampayutto, cittena sampayutto, sārammaṇo;",
  "kv12.3:1.4_0": "atthi tassa āvaṭṭanā ābhogo samannāhāro manasikāro cetanā patthanā paṇidhīti?",
  "kv12.3:1.5_0": "Na hevaṁ vattabbe …pe…",
  "kv12.3:1.6_0": "nanu na sukhavedaniyo na dukkhavedaniyo …pe… anārammaṇo,",
  "kv12.3:1.6_1": "natthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv12.3:1.7_0": "Āmantā.",
  "kv12.3:1.8_0": "Hañci na sukhavedaniyo …pe… anārammaṇo,",
  "kv12.3:1.8_1": "natthi tassa āvaṭṭanā …pe… paṇidhi, no ca vata re vattabbe—",
  "kv12.3:1.9_0": "“saddo vipāko”ti.",
  "kv12.3:2.1_0": "Phasso vipāko,",
  "kv12.3:2.1_1": "phasso sukhavedaniyo …pe… sārammaṇo,",
  "kv12.3:2.1_2": "atthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv12.3:2.2_0": "Āmantā.",
  "kv12.3:2.3_0": "Saddo vipāko,",
  "kv12.3:2.3_1": "saddo sukhavedaniyo …pe… sārammaṇo,",
  "kv12.3:2.3_2": "atthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv12.3:2.4_0": "Na hevaṁ vattabbe …pe….",
  "kv12.3:3.1_0": "Saddo vipāko,",
  "kv12.3:3.1_1": "saddo na sukhavedaniyo …pe… anārammaṇo,",
  "kv12.3:3.1_2": "natthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv12.3:3.2_0": "Āmantā.",
  "kv12.3:3.3_0": "Phasso vipāko, phasso na sukhavedaniyo,",
  "kv12.3:3.3_1": "na dukkhavedaniyo …pe… anārammaṇo,",
  "kv12.3:3.3_2": "natthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv12.3:3.4_0": "Na hevaṁ vattabbe …pe….",
  "kv12.3:4.1_0": "Na vattabbaṁ—",
  "kv12.3:4.2_0": "“saddo vipāko”ti?",
  "kv12.3:4.3_0": "Āmantā.",
  "kv12.3:4.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv12.3:4.5_0": "“so tassa kammassa katattā upacitattā ussannattā vipulattā brahmassaro hoti karavikabhāṇī”ti.",
  "kv12.3:4.6_0": "Attheva suttantoti?",
  "kv12.3:4.7_0": "Āmantā.",
  "kv12.3:4.8_0": "Tena hi saddo vipākoti.",
  "kv12.3:4.9_0": "Saddo vipākotikathā niṭṭhitā."
}