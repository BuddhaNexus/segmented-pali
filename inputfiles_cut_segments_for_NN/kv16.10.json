{
  "kv16.10:0.1_0": "Kathāvatthu",
  "kv16.10:0.2_0": "Mahāpaṇṇāsaka",
  "kv16.10:0.3_0": "Soḷasamavagga",
  "kv16.10:0.4_0": "Rūpārūpadhātupariyāpannakathā",
  "kv16.10:1.1_0": "Rūparāgo rūpadhātupariyāpannoti?",
  "kv16.10:1.2_0": "Āmantā.",
  "kv16.10:1.3_0": "Samāpattesiyo upapattesiyo diṭṭhadhammasukhavihāro,",
  "kv16.10:1.3_1": "samāpattesiyena cittena upapattesiyena cittena diṭṭhadhammasukhavihārena cittena sahagato sahajāto saṁsaṭṭho sampayutto ekuppādo ekanirodho ekavatthuko ekārammaṇoti?",
  "kv16.10:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv16.10:1.5_0": "nanu na samāpattesiyo na upapattesiyo na diṭṭhadhammasukhavihāro,",
  "kv16.10:1.5_1": "na samāpattesiyena cittena …pe… ekavatthuko ekārammaṇoti?",
  "kv16.10:1.6_0": "Āmantā.",
  "kv16.10:1.7_0": "Hañci na samāpattesiyo na upapattesiyo na diṭṭhadhammasukhavihāro,",
  "kv16.10:1.7_1": "na samāpattesiyena cittena …pe… ekavatthuko ekārammaṇo, no ca vata re vattabbe—",
  "kv16.10:1.8_0": "“rūparāgo rūpadhātupariyāpanno”ti.",
  "kv16.10:2.1_0": "Rūparāgo rūpadhātupariyāpannoti?",
  "kv16.10:2.2_0": "Āmantā.",
  "kv16.10:2.3_0": "Saddarāgo saddadhātupariyāpannoti?",
  "kv16.10:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv16.10:2.5_0": "rūparāgo rūpadhātupariyāpannoti?",
  "kv16.10:2.6_0": "Āmantā.",
  "kv16.10:2.7_0": "Gandharāgo …pe…",
  "kv16.10:2.8_0": "rasarāgo …pe…",
  "kv16.10:2.9_0": "phoṭṭhabbarāgo phoṭṭhabbadhātupariyāpannoti?",
  "kv16.10:2.10_0": "Na hevaṁ vattabbe …pe….",
  "kv16.10:3.1_0": "Saddarāgo na vattabbaṁ—",
  "kv16.10:3.2_0": "“saddadhātupariyāpanno”ti?",
  "kv16.10:3.3_0": "Āmantā.",
  "kv16.10:3.4_0": "Rūparāgo na vattabbaṁ—",
  "kv16.10:3.5_0": "“rūpadhātupariyāpanno”ti?",
  "kv16.10:3.6_0": "Na hevaṁ vattabbe …pe…",
  "kv16.10:3.7_0": "gandharāgo …pe…",
  "kv16.10:3.8_0": "rasarāgo …pe…",
  "kv16.10:3.9_0": "phoṭṭhabbarāgo na vattabbaṁ—",
  "kv16.10:3.10_0": "“phoṭṭhabbadhātupariyāpanno”ti?",
  "kv16.10:3.11_0": "Āmantā.",
  "kv16.10:3.12_0": "Rūparāgo na vattabbaṁ—",
  "kv16.10:3.13_0": "“rūpadhātupariyāpanno”ti?",
  "kv16.10:3.14_0": "Na hevaṁ vattabbe …pe….",
  "kv16.10:4.1_0": "Arūparāgo arūpadhātupariyāpannoti?",
  "kv16.10:4.2_0": "Āmantā.",
  "kv16.10:4.3_0": "Arūparāgo na vattabbaṁ—",
  "kv16.10:4.4_0": "“arūpadhātupariyāpanno”ti?",
  "kv16.10:4.5_0": "Na hevaṁ vattabbe …pe…",
  "kv16.10:4.6_0": "arūparāgo arūpadhātupariyāpannoti?",
  "kv16.10:4.7_0": "Āmantā.",
  "kv16.10:4.8_0": "Samāpattesiyo upapattesiyo diṭṭhadhammasukhavihāro,",
  "kv16.10:4.8_1": "samāpattesiyena cittena upapattesiyena cittena diṭṭhadhammasukhavihārena cittena sahagato sahajāto saṁsaṭṭho sampayutto ekuppādo ekanirodho ekavatthuko ekārammaṇoti?",
  "kv16.10:4.9_0": "Na hevaṁ vattabbe …pe…",
  "kv16.10:4.10_0": "nanu na samāpattesiyo na upapattesiyo na diṭṭhadhammasukhavihāro,",
  "kv16.10:4.10_1": "na samāpattesiyena cittena …pe… ekavatthuko ekārammaṇoti?",
  "kv16.10:4.11_0": "Āmantā.",
  "kv16.10:4.12_0": "Hañci na samāpattesiyo na upapattesiyo na diṭṭhadhammasukhavihāro,",
  "kv16.10:4.12_1": "na samāpattesiyena cittena na upapattesiyena cittena na diṭṭhadhammasukhavihārena cittena sahagato sahajāto saṁsaṭṭho sampayutto ekuppādo ekanirodho ekavatthuko ekārammaṇo, no ca vata re vattabbe—",
  "kv16.10:4.13_0": "“arūparāgo arūpadhātupariyāpanno”ti.",
  "kv16.10:5.1_0": "Arūparāgo arūpadhātupariyāpannoti?",
  "kv16.10:5.2_0": "Āmantā.",
  "kv16.10:5.3_0": "Saddarāgo saddadhātupariyāpannoti?",
  "kv16.10:5.4_0": "Na hevaṁ vattabbe …pe…",
  "kv16.10:5.5_0": "arūparāgo arūpadhātupariyāpannoti?",
  "kv16.10:5.6_0": "Āmantā.",
  "kv16.10:5.7_0": "Gandharāgo …pe…",
  "kv16.10:5.8_0": "rasarāgo …pe…",
  "kv16.10:5.9_0": "phoṭṭhabbarāgo phoṭṭhabbadhātupariyāpannoti?",
  "kv16.10:5.10_0": "Na hevaṁ vattabbe …pe….",
  "kv16.10:6.1_0": "Saddarāgo na vattabbaṁ—",
  "kv16.10:6.2_0": "“saddadhātupariyāpanno”ti?",
  "kv16.10:6.3_0": "Āmantā.",
  "kv16.10:6.4_0": "Arūparāgo na vattabbaṁ—",
  "kv16.10:6.5_0": "“arūpadhātupariyāpanno”ti?",
  "kv16.10:6.6_0": "Na hevaṁ vattabbe …pe…",
  "kv16.10:6.7_0": "gandharāgo …pe…",
  "kv16.10:6.8_0": "rasarāgo …pe…",
  "kv16.10:6.9_0": "phoṭṭhabbarāgo na vattabbaṁ—",
  "kv16.10:6.10_0": "“phoṭṭhabbadhātupariyāpanno”ti?",
  "kv16.10:6.11_0": "Āmantā.",
  "kv16.10:6.12_0": "Arūparāgo na vattabbaṁ—",
  "kv16.10:6.13_0": "“arūpadhātupariyāpanno”ti?",
  "kv16.10:6.14_0": "Na hevaṁ vattabbe …pe….",
  "kv16.10:7.1_0": "Na vattabbaṁ—",
  "kv16.10:7.2_0": "“rūparāgo rūpadhātupariyāpanno,",
  "kv16.10:7.2_1": "arūparāgo arūpadhātupariyāpanno”ti?",
  "kv16.10:7.3_0": "Āmantā.",
  "kv16.10:7.4_0": "Nanu kāmarāgo kāmadhātupariyāpannoti?",
  "kv16.10:7.5_0": "Āmantā.",
  "kv16.10:7.6_0": "Hañci kāmarāgo kāmadhātupariyāpanno, tena vata re vattabbe—",
  "kv16.10:7.7_0": "“rūparāgo rūpadhātupariyāpanno,",
  "kv16.10:7.7_1": "arūparāgo arūpadhātupariyāpanno”ti.",
  "kv16.10:7.8_0": "Rūparāgo rūpadhātupariyāpanno arūparāgo",
  "kv16.10:7.9_0": "arūpadhātupariyāpannotikathā niṭṭhitā.",
  "kv16.10:7.10_0": "Soḷasamo vaggo.",
  "kv16.10:7.11_0": "Tassuddānaṁ",
  "kv16.10:8.1_0": "Cittaniggaho, cittapaggaho, sukhānuppadānaṁ, adhigayha manasikāro, rūpaṁ hetu, rūpaṁ sahetukaṁ, rūpaṁ kusalampi akusalampi, rūpaṁ vipāko,",
  "kv16.10:8.1_1": "atthi rūpaṁ rūpāvacaraṁ atthi rūpaṁ arūpāvacaraṁ,",
  "kv16.10:8.1_2": "sabbe kilesā kāmadhātupariyāpannāti."
}