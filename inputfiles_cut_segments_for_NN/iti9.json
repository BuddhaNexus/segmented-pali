{
  "iti9:0.1_0": "Itivuttaka 9",
  "iti9:0.2_0": "Ekakanipāta",
  "iti9:0.3_0": "Paṭhamavagga",
  "iti9:0.4_0": "Lobhapariññāsutta",
  "iti9:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti9:2.1_0": "“Lobhaṁ,",
  "iti9:2.1_1": "bhikkhave,",
  "iti9:2.1_2": "anabhijānaṁ aparijānaṁ tattha cittaṁ avirājayaṁ appajahaṁ abhabbo dukkhakkhayāya.",
  "iti9:2.2_0": "Lobhañca kho,",
  "iti9:2.2_1": "bhikkhave,",
  "iti9:2.2_2": "abhijānaṁ parijānaṁ tattha cittaṁ virājayaṁ pajahaṁ bhabbo dukkhakkhayāyā”ti.",
  "iti9:2.3_0": "Etamatthaṁ bhagavā avoca.",
  "iti9:2.4_0": "Tatthetaṁ iti vuccati:",
  "iti9:3.1_0": "“Yena lobhena luddhāse,",
  "iti9:3.2_0": "sattā gacchanti duggatiṁ;",
  "iti9:3.3_0": "Taṁ lobhaṁ sammadaññāya,",
  "iti9:3.4_0": "pajahanti vipassino;",
  "iti9:3.5_0": "Pahāya na punāyanti,",
  "iti9:3.6_0": "imaṁ lokaṁ kudācanan”ti.",
  "iti9:4.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti9:4.2_0": "Navamaṁ."
}