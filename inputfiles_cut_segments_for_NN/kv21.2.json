{
  "kv21.2:0.1_0": "Kathāvatthu",
  "kv21.2:0.2_0": "Khuddakaaḍḍhapaṇṇāsaka",
  "kv21.2:0.3_0": "Ekavīsatimavagga",
  "kv21.2:0.4_0": "Avivittakathā",
  "kv21.2:1.1_0": "Puthujjano tedhātukehi dhammehi avivittoti?",
  "kv21.2:1.2_0": "Āmantā.",
  "kv21.2:1.3_0": "Puthujjano tedhātukehi phassehi …pe…",
  "kv21.2:1.4_0": "tedhātukāhi vedanāhi …",
  "kv21.2:1.5_0": "saññāhi …",
  "kv21.2:1.6_0": "cetanāhi …",
  "kv21.2:1.7_0": "cittehi …",
  "kv21.2:1.8_0": "saddhāhi …",
  "kv21.2:1.9_0": "vīriyehi …",
  "kv21.2:1.10_0": "satīhi …",
  "kv21.2:1.11_0": "samādhīhi …pe…",
  "kv21.2:1.12_0": "tedhātukāhi paññāhi avivittoti?",
  "kv21.2:1.13_0": "Na hevaṁ vattabbe …pe….",
  "kv21.2:2.1_0": "Puthujjano tedhātukehi kammehi avivittoti?",
  "kv21.2:2.2_0": "Āmantā.",
  "kv21.2:2.3_0": "Yasmiṁ khaṇe puthujjano cīvaraṁ deti,",
  "kv21.2:2.3_1": "tasmiṁ khaṇe paṭhamaṁ jhānaṁ upasampajja viharati …pe…",
  "kv21.2:2.4_0": "ākāsānañcāyatanaṁ upasampajja viharatīti?",
  "kv21.2:2.5_0": "Na hevaṁ vattabbe …pe…",
  "kv21.2:2.6_0": "yasmiṁ khaṇe puthujjano piṇḍapātaṁ deti …pe…",
  "kv21.2:2.7_0": "senāsanaṁ deti …pe…",
  "kv21.2:2.8_0": "gilānapaccayabhesajjaparikkhāraṁ deti,",
  "kv21.2:2.8_1": "tasmiṁ khaṇe catutthaṁ jhānaṁ upasampajja viharati …",
  "kv21.2:2.9_0": "nevasaññānāsaññāyatanaṁ upasampajja viharatīti?",
  "kv21.2:2.10_0": "Na hevaṁ vattabbe …pe….",
  "kv21.2:3.1_0": "Na vattabbaṁ—",
  "kv21.2:3.2_0": "“puthujjano tedhātukehi kammehi avivitto”ti?",
  "kv21.2:3.3_0": "Āmantā.",
  "kv21.2:3.4_0": "Puthujjanassa rūpadhātuarūpadhātūpagaṁ kammaṁ pariññātanti?",
  "kv21.2:3.5_0": "Na hevaṁ vattabbe.",
  "kv21.2:3.6_0": "Tena hi puthujjano tedhātukehi kammehi avivittoti …pe….",
  "kv21.2:3.7_0": "Avivittakathā niṭṭhitā."
}