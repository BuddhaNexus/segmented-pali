{
  "pli-tv-bi-vb-as1:0.1_0": "Theravāda Vinaya",
  "pli-tv-bi-vb-as1:0.2_0": "Bhikkhunivibhaṅga",
  "pli-tv-bi-vb-as1:0.3_0": "Adhikaraṇasamatha",
  "pli-tv-bi-vb-as1:1.1_0": "Ime kho panāyyāyo satta adhikaraṇasamathā dhammā uddesaṁ āgacchanti.",
  "pli-tv-bi-vb-as1:2.1_0": "Uppannuppannānaṁ adhikaraṇānaṁ samathāya vūpasamāya",
  "pli-tv-bi-vb-as1:2.2_0": "sammukhāvinayo dātabbo,",
  "pli-tv-bi-vb-as2:1.1_0": "sativinayo dātabbo,",
  "pli-tv-bi-vb-as3:1.1_0": "amūḷhavinayo dātabbo,",
  "pli-tv-bi-vb-as4:1.1_0": "paṭiññāya kāretabbaṁ,",
  "pli-tv-bi-vb-as5:1.1_0": "yebhuyyasikā,",
  "pli-tv-bi-vb-as6:1.1_0": "tassapāpiyasikā,",
  "pli-tv-bi-vb-as7:1.1_0": "tiṇavatthārakoti.",
  "pli-tv-bi-vb-as7:2.1_0": "Uddiṭṭhā kho, ayyāyo,",
  "pli-tv-bi-vb-as7:2.1_1": "satta adhikaraṇasamathā dhammā.",
  "pli-tv-bi-vb-as7:2.2_0": "Tatthāyyāyo pucchāmi—",
  "pli-tv-bi-vb-as7:2.3_0": "“kaccittha parisuddhā”?",
  "pli-tv-bi-vb-as7:2.4_0": "Dutiyampi pucchāmi—",
  "pli-tv-bi-vb-as7:2.5_0": "“kaccittha parisuddhā”?",
  "pli-tv-bi-vb-as7:2.6_0": "Tatiyampi pucchāmi—",
  "pli-tv-bi-vb-as7:2.7_0": "“kaccittha parisuddhā”?",
  "pli-tv-bi-vb-as7:2.8_0": "Parisuddhetthāyyāyo, tasmā tuṇhī, evametaṁ dhārayāmīti.",
  "pli-tv-bi-vb-as7:3.1_0": "Adhikaraṇasamathā niṭṭhitā.",
  "pli-tv-bi-vb-as7:4.1_0": "Uddiṭṭhaṁ kho, ayyāyo, nidānaṁ.",
  "pli-tv-bi-vb-as7:4.2_0": "Uddiṭṭhā aṭṭha pārājikā dhammā.",
  "pli-tv-bi-vb-as7:4.3_0": "Uddiṭṭhā sattarasa saṅghādisesā dhammā.",
  "pli-tv-bi-vb-as7:4.4_0": "Uddiṭṭhā tiṁsa nissaggiyā pācittiyā dhammā.",
  "pli-tv-bi-vb-as7:4.5_0": "Uddiṭṭhā chasaṭṭhisatā pācittiyā dhammā.",
  "pli-tv-bi-vb-as7:4.6_0": "Uddiṭṭhā aṭṭha pāṭidesanīyā dhammā.",
  "pli-tv-bi-vb-as7:4.7_0": "Uddiṭṭhā sekhiyā dhammā.",
  "pli-tv-bi-vb-as7:4.8_0": "Uddiṭṭhā satta adhikaraṇasamathā dhammā.",
  "pli-tv-bi-vb-as7:4.9_0": "Ettakaṁ tassa bhagavato suttāgataṁ suttapariyāpannaṁ anvaddhamāsaṁ uddesaṁ āgacchati.",
  "pli-tv-bi-vb-as7:4.10_0": "Tattha sabbāheva samaggāhi sammodamānāhi avivadamānāhi sikkhitabbanti.",
  "pli-tv-bi-vb-as7:4.11_0": "Bhikkhunivibhaṅgo niṭṭhito.",
  "pli-tv-bi-vb-as7:5.1_0": "Pācittiyapāḷi niṭṭhitā."
}