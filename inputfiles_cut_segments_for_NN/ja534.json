{
  "ja534:0.1_0": "Jātaka",
  "ja534:0.2_0": "Asītinipāta",
  "ja534:0.3_0": "Cūḷahaṁsavagga",
  "ja534:0.4_0": "2. Mahāhaṁsajātaka",
  "ja534:1.1_0": "“Ete haṁsā pakkamanti,",
  "ja534:1.2_0": "vakkaṅgā bhayameritā;",
  "ja534:1.3_0": "Harittaca hemavaṇṇa,",
  "ja534:1.4_0": "kāmaṁ sumukha pakkama.",
  "ja534:2.1_0": "Ohāya maṁ ñātigaṇā,",
  "ja534:2.2_0": "ekaṁ pāsavasaṁ gataṁ;",
  "ja534:2.3_0": "Anapekkhamānā gacchanti,",
  "ja534:2.4_0": "kiṁ eko avahīyasi.",
  "ja534:3.1_0": "Pateva patataṁ seṭṭha,",
  "ja534:3.2_0": "natthi baddhe sahāyatā;",
  "ja534:3.3_0": "Mā anīghāya hāpesi,",
  "ja534:3.4_0": "kāmaṁ sumukha pakkama”.",
  "ja534:4.1_0": "“Nāhaṁ dukkhaparetopi,",
  "ja534:4.2_0": "dhataraṭṭha tuvaṁ jahe;",
  "ja534:4.3_0": "Jīvitaṁ maraṇaṁ vā me,",
  "ja534:4.4_0": "tayā saddhiṁ bhavissati.",
  "ja534:5.1_0": "Nāhaṁ dukkhaparetopi,",
  "ja534:5.2_0": "dhataraṭṭha tuvaṁ jahe;",
  "ja534:5.3_0": "Na maṁ anariyasaṁyutte,",
  "ja534:5.4_0": "kamme yojetumarahasi.",
  "ja534:6.1_0": "Sakumāro sakhā tyasmi,",
  "ja534:6.2_0": "sacitte casmi te ṭhito;",
  "ja534:6.3_0": "Ñāto senāpati tyāhaṁ,",
  "ja534:6.4_0": "haṁsānaṁ pavaruttama.",
  "ja534:7.1_0": "Kathaṁ ahaṁ vikatthissaṁ,",
  "ja534:7.2_0": "ñātimajjhe ito gato;",
  "ja534:7.3_0": "Taṁ hitvā patataṁ seṭṭha,",
  "ja534:7.4_0": "kiṁ te vakkhāmito gato;",
  "ja534:7.5_0": "Idha pāṇaṁ cajissāmi,",
  "ja534:7.6_0": "nānariyaṁ kattumussahe”.",
  "ja534:8.1_0": "“Eso hi dhammo sumukha,",
  "ja534:8.2_0": "yaṁ tvaṁ ariyapathe ṭhito;",
  "ja534:8.3_0": "Yo bhattāraṁ sakhāraṁ maṁ,",
  "ja534:8.4_0": "na pariccattumussahe.",
  "ja534:9.1_0": "Tañhi me pekkhamānassa,",
  "ja534:9.2_0": "bhayaṁ na tveva jāyati;",
  "ja534:9.3_0": "Adhigacchasi tvaṁ mayhaṁ,",
  "ja534:9.4_0": "evaṁ bhūtassa jīvitaṁ”.",
  "ja534:10.1_0": "Iccevaṁ mantayantānaṁ,",
  "ja534:10.2_0": "Ariyānaṁ ariyavuttinaṁ;",
  "ja534:10.3_0": "Daṇḍamādāya nesādo,",
  "ja534:10.4_0": "Āpatī turito bhusaṁ.",
  "ja534:11.1_0": "Tamāpatantaṁ disvāna,",
  "ja534:11.2_0": "Sumukho atibrūhayi;",
  "ja534:11.3_0": "Aṭṭhāsi purato rañño,",
  "ja534:11.4_0": "Haṁso vissāsayaṁ byathaṁ.",
  "ja534:12.1_0": "“Mā bhāyi patataṁ seṭṭha,",
  "ja534:12.2_0": "na hi bhāyanti tādisā;",
  "ja534:12.3_0": "Ahaṁ yogaṁ payuñjissaṁ,",
  "ja534:12.4_0": "yuttaṁ dhammūpasaṁhitaṁ;",
  "ja534:12.5_0": "Tena pariyāpadānena,",
  "ja534:12.6_0": "khippaṁ pāsā pamokkhasi”.",
  "ja534:13.1_0": "Tassa taṁ vacanaṁ sutvā,",
  "ja534:13.2_0": "sumukhassa subhāsitaṁ;",
  "ja534:13.3_0": "Pahaṭṭhalomo nesādo,",
  "ja534:13.4_0": "añjalissa paṇāmayi.",
  "ja534:14.1_0": "“Na me sutaṁ vā diṭṭhaṁ vā,",
  "ja534:14.2_0": "bhāsanto mānusiṁ dijo;",
  "ja534:14.3_0": "Ariyaṁ bruvāno vakkaṅgo,",
  "ja534:14.4_0": "cajanto mānusiṁ giraṁ.",
  "ja534:15.1_0": "Kiṁ nu tāyaṁ dijo hoti,",
  "ja534:15.2_0": "mutto baddhaṁ upāsasi;",
  "ja534:15.3_0": "Ohāya sakuṇā yanti,",
  "ja534:15.4_0": "kiṁ eko avahīyasi”.",
  "ja534:16.1_0": "“Rājā me so dijāmitta,",
  "ja534:16.2_0": "senāpaccassa kārayiṁ;",
  "ja534:16.3_0": "Tamāpade pariccattuṁ,",
  "ja534:16.4_0": "nussahe vihagādhipaṁ.",
  "ja534:17.1_0": "Mahāgaṇāya bhattā me,",
  "ja534:17.2_0": "mā eko byasanaṁ agā;",
  "ja534:17.3_0": "Tathā taṁ samma nesāda,",
  "ja534:17.4_0": "bhattāyaṁ abhito rame”.",
  "ja534:18.1_0": "“Ariyavattasi vakkaṅga,",
  "ja534:18.2_0": "yo piṇḍamapacāyasi;",
  "ja534:18.3_0": "Cajāmi te taṁ bhattāraṁ,",
  "ja534:18.4_0": "gacchathūbho yathāsukhaṁ”.",
  "ja534:19.1_0": "“Sace attappayogena,",
  "ja534:19.2_0": "ohito haṁsapakkhinaṁ;",
  "ja534:19.3_0": "Paṭigaṇhāma te samma,",
  "ja534:19.4_0": "etaṁ abhayadakkhiṇaṁ.",
  "ja534:20.1_0": "No ce attappayogena,",
  "ja534:20.2_0": "ohito haṁsapakkhinaṁ;",
  "ja534:20.3_0": "Anissaro muñcamamhe,",
  "ja534:20.4_0": "theyyaṁ kayirāsi luddaka”.",
  "ja534:21.1_0": "“Yassa tvaṁ bhatako rañño,",
  "ja534:21.2_0": "kāmaṁ tasseva pāpaya;",
  "ja534:21.3_0": "Tattha saṁyamano rājā,",
  "ja534:21.4_0": "yathābhiññaṁ karissati”.",
  "ja534:22.1_0": "Iccevaṁ vutto nesādo,",
  "ja534:22.2_0": "hemavaṇṇe harittace;",
  "ja534:22.3_0": "Ubho hatthehi saṅgayha,",
  "ja534:22.4_0": "pañjare ajjhavodahi.",
  "ja534:23.1_0": "Te pañjaragate pakkhī,",
  "ja534:23.2_0": "ubho bhassaravaṇṇine;",
  "ja534:23.3_0": "Sumukhaṁ dhataraṭṭhañca,",
  "ja534:23.4_0": "luddo ādāya pakkami.",
  "ja534:24.1_0": "Harīyamāno dhataraṭṭho,",
  "ja534:24.2_0": "sumukhaṁ etadabravi;",
  "ja534:24.3_0": "“Bāḷhaṁ bhāyāmi sumukha,",
  "ja534:24.4_0": "sāmāya lakkhaṇūruyā;",
  "ja534:24.5_0": "Asmākaṁ vadhamaññāya,",
  "ja534:24.6_0": "athattānaṁ vadhissati.",
  "ja534:25.1_0": "Pākahaṁsā ca sumukha,",
  "ja534:25.2_0": "suhemā hemasuttacā;",
  "ja534:25.3_0": "Koñcī samuddatīreva,",
  "ja534:25.4_0": "kapaṇā nūna rucchati”.",
  "ja534:26.1_0": "“Evaṁ mahanto lokassa,",
  "ja534:26.2_0": "appameyyo mahāgaṇī;",
  "ja534:26.3_0": "Ekitthimanusoceyya,",
  "ja534:26.4_0": "nayidaṁ paññavatāmiva.",
  "ja534:27.1_0": "Vātova gandhamādeti,",
  "ja534:27.2_0": "ubhayaṁ chekapāpakaṁ;",
  "ja534:27.3_0": "Bālo āmakapakkaṁva,",
  "ja534:27.4_0": "lolo andhova āmisaṁ.",
  "ja534:28.1_0": "Avinicchayaññu atthesu,",
  "ja534:28.2_0": "mandova paṭibhāsi maṁ;",
  "ja534:28.3_0": "Kiccākiccaṁ na jānāsi,",
  "ja534:28.4_0": "sampatto kālapariyāyaṁ.",
  "ja534:29.1_0": "Aḍḍhummatto udīresi,",
  "ja534:29.2_0": "yo seyyā maññasitthiyo;",
  "ja534:29.3_0": "Bahusādhāraṇā hetā,",
  "ja534:29.4_0": "soṇḍānaṁva surāgharaṁ.",
  "ja534:30.1_0": "Māyā cesā marīcī ca,",
  "ja534:30.2_0": "soko rogo cupaddavo;",
  "ja534:30.3_0": "Kharā ca bandhanā cetā,",
  "ja534:30.4_0": "maccupāsā guhāsayā;",
  "ja534:30.5_0": "Tāsu yo vissase poso,",
  "ja534:30.6_0": "so naresu narādhamo”.",
  "ja534:31.1_0": "“Yaṁ vuddhehi upaññātaṁ,",
  "ja534:31.2_0": "ko taṁ ninditumarahati;",
  "ja534:31.3_0": "Mahābhūtitthiyo nāma,",
  "ja534:31.4_0": "lokasmiṁ udapajjisuṁ.",
  "ja534:32.1_0": "Khiḍḍā paṇihitā tyāsu,",
  "ja534:32.2_0": "rati tyāsu patiṭṭhitā;",
  "ja534:32.3_0": "Bījāni tyāsu rūhanti,",
  "ja534:32.4_0": "yadidaṁ sattā pajāyare;",
  "ja534:32.5_0": "Tāsu ko nibbide poso,",
  "ja534:32.6_0": "pāṇamāsajja pāṇibhi.",
  "ja534:33.1_0": "Tvameva nañño sumukha,",
  "ja534:33.2_0": "thīnaṁ atthesu yuñjasi;",
  "ja534:33.3_0": "Tassa tyajja bhaye jāte,",
  "ja534:33.4_0": "bhītena jāyate mati.",
  "ja534:34.1_0": "Sabbo hi saṁsayaṁ patto,",
  "ja534:34.2_0": "bhayaṁ bhīru titikkhati;",
  "ja534:34.3_0": "Paṇḍitā ca mahantāno,",
  "ja534:34.4_0": "atthe yuñjanti duyyuje.",
  "ja534:35.1_0": "Etadatthāya rājāno,",
  "ja534:35.2_0": "sūramicchanti mantinaṁ;",
  "ja534:35.3_0": "Paṭibāhati yaṁ sūro,",
  "ja534:35.4_0": "āpadaṁ attapariyāyaṁ.",
  "ja534:36.1_0": "Mā no ajja vikantiṁsu,",
  "ja534:36.2_0": "rañño sūdā mahānase;",
  "ja534:36.3_0": "Tathā hi vaṇṇo pattānaṁ,",
  "ja534:36.4_0": "phalaṁ veḷuṁva taṁ vadhi.",
  "ja534:37.1_0": "Muttopi na icchi uḍḍetuṁ,",
  "ja534:37.2_0": "Sayaṁ bandhaṁ upāgami;",
  "ja534:37.3_0": "Sopajja saṁsayaṁ patto,",
  "ja534:37.4_0": "Atthaṁ gaṇhāhi mā mukhaṁ.",
  "ja534:38.1_0": "So taṁ yogaṁ payuñjassu,",
  "ja534:38.2_0": "yuttaṁ dhammūpasaṁhitaṁ;",
  "ja534:38.3_0": "Tava pariyāpadānena,",
  "ja534:38.4_0": "mama pāṇesanaṁ cara”.",
  "ja534:39.1_0": "“Mā bhāyi patataṁ seṭṭha,",
  "ja534:39.2_0": "na hi bhāyanti tādisā;",
  "ja534:39.3_0": "Ahaṁ yogaṁ payuñjissaṁ,",
  "ja534:39.4_0": "yuttaṁ dhammūpasaṁhitaṁ;",
  "ja534:39.5_0": "Mama pariyāpadānena,",
  "ja534:39.6_0": "khippaṁ pāsā pamokkhasi”.",
  "ja534:40.1_0": "So luddo haṁsakājena,",
  "ja534:40.2_0": "rājadvāraṁ upāgami;",
  "ja534:40.3_0": "“Paṭivedetha maṁ rañño,",
  "ja534:40.4_0": "dhataraṭṭhāyamāgato”.",
  "ja534:41.1_0": "Te disvā puññasaṅkāse,",
  "ja534:41.2_0": "ubho lakkhaṇasammate;",
  "ja534:41.3_0": "Khalu saṁyamano rājā,",
  "ja534:41.4_0": "amacce ajjhabhāsatha.",
  "ja534:42.1_0": "“Detha luddassa vatthāni,",
  "ja534:42.2_0": "annaṁ pānañca bhojanaṁ;",
  "ja534:42.3_0": "Kāmaṅkaro hiraññassa,",
  "ja534:42.4_0": "yāvanto esa icchati”.",
  "ja534:43.1_0": "Disvā luddaṁ pasannattaṁ,",
  "ja534:43.2_0": "Kāsirājā tadabravi;",
  "ja534:43.3_0": "“Yadyāyaṁ samma khemaka,",
  "ja534:43.4_0": "Puṇṇā haṁsehi tiṭṭhati.",
  "ja534:44.1_0": "Kathaṁ rucimajjhagataṁ,",
  "ja534:44.2_0": "pāsahattho upāgami;",
  "ja534:44.3_0": "Okiṇṇaṁ ñātisaṅghehi,",
  "ja534:44.4_0": "nimmajjhimaṁ kathaṁ gahi”.",
  "ja534:45.1_0": "“Ajja me sattamā ratti,",
  "ja534:45.2_0": "adanāni upāsato;",
  "ja534:45.3_0": "Padametassa anvesaṁ,",
  "ja534:45.4_0": "appamatto ghaṭassito.",
  "ja534:46.1_0": "Athassa padamaddakkhiṁ,",
  "ja534:46.2_0": "carato adanesanaṁ;",
  "ja534:46.3_0": "Tatthāhaṁ odahiṁ pāsaṁ,",
  "ja534:46.4_0": "evaṁ taṁ dijamaggahiṁ”.",
  "ja534:47.1_0": "“Ludda dve ime sakuṇā,",
  "ja534:47.2_0": "atha ekoti bhāsasi;",
  "ja534:47.3_0": "Cittaṁ nu te vipariyattaṁ,",
  "ja534:47.4_0": "adu kiṁ nu jigīsasi”.",
  "ja534:48.1_0": "“Yassa lohitakā tālā,",
  "ja534:48.2_0": "tapanīyanibhā subhā;",
  "ja534:48.3_0": "Uraṁ saṁhacca tiṭṭhanti,",
  "ja534:48.4_0": "so me bandhaṁ upāgami.",
  "ja534:49.1_0": "Athāyaṁ bhassaro pakkhī,",
  "ja534:49.2_0": "abaddho baddhamāturaṁ;",
  "ja534:49.3_0": "Ariyaṁ bruvāno aṭṭhāsi,",
  "ja534:49.4_0": "cajanto mānusiṁ giraṁ”.",
  "ja534:50.1_0": "“Atha kiṁ dāni sumukha,",
  "ja534:50.2_0": "Hanuṁ saṁhacca tiṭṭhasi;",
  "ja534:50.3_0": "Adu me parisaṁ patto,",
  "ja534:50.4_0": "Bhayā bhīto na bhāsasi”.",
  "ja534:51.1_0": "“Nāhaṁ kāsipati bhīto,",
  "ja534:51.2_0": "ogayha parisaṁ tava;",
  "ja534:51.3_0": "Nāhaṁ bhayā na bhāsissaṁ,",
  "ja534:51.4_0": "vākyaṁ atthamhi tādise”.",
  "ja534:52.1_0": "“Na te abhisaraṁ passe,",
  "ja534:52.2_0": "na rathe napi pattike;",
  "ja534:52.3_0": "Nāssa cammaṁ va kīṭaṁ vā,",
  "ja534:52.4_0": "vammite ca dhanuggahe.",
  "ja534:53.1_0": "Na hiraññaṁ suvaṇṇaṁ vā,",
  "ja534:53.2_0": "nagaraṁ vā sumāpitaṁ;",
  "ja534:53.3_0": "Okiṇṇaparikhaṁ duggaṁ,",
  "ja534:53.4_0": "daḷhamaṭṭālakoṭṭhakaṁ;",
  "ja534:53.5_0": "Yattha paviṭṭho sumukha,",
  "ja534:53.6_0": "bhāyitabbaṁ na bhāyasi”.",
  "ja534:54.1_0": "“Na me abhisarenattho,",
  "ja534:54.2_0": "nagarena dhanena vā;",
  "ja534:54.3_0": "Apathena pathaṁ yāma,",
  "ja534:54.4_0": "antalikkhecarā mayaṁ.",
  "ja534:55.1_0": "Sutā ca paṇḍitā tyamhā,",
  "ja534:55.2_0": "nipuṇā atthacintakā;",
  "ja534:55.3_0": "Bhāsematthavatiṁ vācaṁ,",
  "ja534:55.4_0": "sacce cassa patiṭṭhito.",
  "ja534:56.1_0": "Kiñca tuyhaṁ asaccassa,",
  "ja534:56.2_0": "anariyassa karissati;",
  "ja534:56.3_0": "Musāvādissa luddassa,",
  "ja534:56.4_0": "bhaṇitampi subhāsitaṁ.",
  "ja534:57.1_0": "Taṁ brāhmaṇānaṁ vacanā,",
  "ja534:57.2_0": "imaṁ khemamakārayi;",
  "ja534:57.3_0": "Abhayañca tayā ghuṭṭhaṁ,",
  "ja534:57.4_0": "imāyo dasadhā disā.",
  "ja534:58.1_0": "Ogayha te pokkharaṇiṁ,",
  "ja534:58.2_0": "vippasannodakaṁ suciṁ;",
  "ja534:58.3_0": "Pahūtaṁ cādanaṁ tattha,",
  "ja534:58.4_0": "ahiṁsā cettha pakkhinaṁ.",
  "ja534:59.1_0": "Idaṁ sutvāna nigghosaṁ,",
  "ja534:59.2_0": "āgatamha tavantike;",
  "ja534:59.3_0": "Te te bandhasma pāsena,",
  "ja534:59.4_0": "etaṁ te bhāsitaṁ musā.",
  "ja534:60.1_0": "Musāvādaṁ purakkhatvā,",
  "ja534:60.2_0": "icchālobhañca pāpakaṁ;",
  "ja534:60.3_0": "Ubho sandhimatikkamma,",
  "ja534:60.4_0": "asātaṁ upapajjati”.",
  "ja534:61.1_0": "“Nāparajjhāma sumukha,",
  "ja534:61.2_0": "napi lobhāva maggahiṁ;",
  "ja534:61.3_0": "Sutā ca paṇḍitātyattha-",
  "ja534:61.4_0": "nipuṇā atthacintakā.",
  "ja534:62.1_0": "Appevatthavatiṁ vācaṁ,",
  "ja534:62.2_0": "byāhareyyuṁ idhāgatā;",
  "ja534:62.3_0": "Tathā taṁ samma nesādo,",
  "ja534:62.4_0": "vutto sumukha maggahi”.",
  "ja534:63.1_0": "“Neva bhītā kāsipati,",
  "ja534:63.2_0": "upanītasmi jīvite;",
  "ja534:63.3_0": "Bhāsematthavatiṁ vācaṁ,",
  "ja534:63.4_0": "sampattā kālapariyāyaṁ.",
  "ja534:64.1_0": "Yo migena migaṁ hanti,",
  "ja534:64.2_0": "pakkhiṁ vā pana pakkhinā;",
  "ja534:64.3_0": "Sutena vā sutaṁ kiṇyā,",
  "ja534:64.4_0": "kiṁ anariyataraṁ tato.",
  "ja534:65.1_0": "Yo cāriyarudaṁ bhāse,",
  "ja534:65.2_0": "anariyadhammavassito;",
  "ja534:65.3_0": "Ubho so dhaṁsate lokā,",
  "ja534:65.4_0": "idha ceva parattha ca.",
  "ja534:66.1_0": "Na majjetha yasaṁ patto,",
  "ja534:66.2_0": "na byādhe pattasaṁsayaṁ;",
  "ja534:66.3_0": "Vāyametheva kiccesu,",
  "ja534:66.4_0": "saṁvare vivarāni ca.",
  "ja534:67.1_0": "Ye vuddhā abbhatikkantā,",
  "ja534:67.2_0": "sampattā kālapariyāyaṁ;",
  "ja534:67.3_0": "Idha dhammaṁ caritvāna,",
  "ja534:67.4_0": "evaṁte tidivaṁ gatā.",
  "ja534:68.1_0": "Idaṁ sutvā kāsipati,",
  "ja534:68.2_0": "dhammamattani pālaya;",
  "ja534:68.3_0": "Dhataraṭṭhañca muñcāhi,",
  "ja534:68.4_0": "haṁsānaṁ pavaruttamaṁ”.",
  "ja534:69.1_0": "“Āharantudakaṁ pajjaṁ,",
  "ja534:69.2_0": "āsanañca mahārahaṁ;",
  "ja534:69.3_0": "Pañjarato pamokkhāmi,",
  "ja534:69.4_0": "dhataraṭṭhaṁ yasassinaṁ.",
  "ja534:70.1_0": "Tañca senāpatiṁ dhīraṁ,",
  "ja534:70.2_0": "nipuṇaṁ atthacintakaṁ;",
  "ja534:70.3_0": "Yo sukhe sukhito rañño,",
  "ja534:70.4_0": "dukkhite hoti dukkhito.",
  "ja534:71.1_0": "Ediso kho arahati,",
  "ja534:71.2_0": "piṇḍamasnātu bhattuno;",
  "ja534:71.3_0": "Yathāyaṁ sumukho rañño,",
  "ja534:71.4_0": "pāṇasādhāraṇo sakhā”.",
  "ja534:72.1_0": "Pīṭhañca sabbasovaṇṇaṁ,",
  "ja534:72.2_0": "Aṭṭhapādaṁ manoramaṁ;",
  "ja534:72.3_0": "Maṭṭhaṁ kāsikamatthannaṁ,",
  "ja534:72.4_0": "Dhataraṭṭho upāvisi.",
  "ja534:73.1_0": "Kocchañca sabbasovaṇṇaṁ,",
  "ja534:73.2_0": "veyyagghaparisibbitaṁ;",
  "ja534:73.3_0": "Sumukho ajjhupāvekkhi,",
  "ja534:73.4_0": "dhataraṭṭhassanantarā.",
  "ja534:74.1_0": "Tesaṁ kañcanapattehi,",
  "ja534:74.2_0": "puthū ādāya kāsiyo;",
  "ja534:74.3_0": "Haṁsānaṁ abhihāresuṁ,",
  "ja534:74.4_0": "aggarañño pavāsitaṁ.",
  "ja534:75.1_0": "Disvā abhihaṭaṁ aggaṁ,",
  "ja534:75.2_0": "kāsirājena pesitaṁ;",
  "ja534:75.3_0": "Kusalo khattadhammānaṁ,",
  "ja534:75.4_0": "tato pucchi anantarā.",
  "ja534:76.1_0": "“Kaccinnu bhoto kusalaṁ,",
  "ja534:76.2_0": "kacci bhoto anāmayaṁ;",
  "ja534:76.3_0": "Kacci raṭṭhamidaṁ phītaṁ,",
  "ja534:76.4_0": "dhammena manusāsasi”.",
  "ja534:77.1_0": "“Kusalañceva me haṁsa,",
  "ja534:77.2_0": "atho haṁsa anāmayaṁ;",
  "ja534:77.3_0": "Atho raṭṭhamidaṁ phītaṁ,",
  "ja534:77.4_0": "dhammenaṁ manusāsahaṁ”.",
  "ja534:78.1_0": "“Kacci bhoto amaccesu,",
  "ja534:78.2_0": "doso koci na vijjati;",
  "ja534:78.3_0": "Kacci ca te tavatthesu,",
  "ja534:78.4_0": "nāvakaṅkhanti jīvitaṁ”.",
  "ja534:79.1_0": "“Athopi me amaccesu,",
  "ja534:79.2_0": "doso koci na vijjati;",
  "ja534:79.3_0": "Athopi te mamatthesu,",
  "ja534:79.4_0": "nāvakaṅkhanti jīvitaṁ”.",
  "ja534:80.1_0": "“Kacci te sādisī bhariyā,",
  "ja534:80.2_0": "assavā piyabhāṇinī;",
  "ja534:80.3_0": "Puttarūpayasūpetā,",
  "ja534:80.4_0": "tava chandavasānugā”.",
  "ja534:81.1_0": "“Atho me sādisī bhariyā,",
  "ja534:81.2_0": "assavā piyabhāṇinī;",
  "ja534:81.3_0": "Puttarūpayasūpetā,",
  "ja534:81.4_0": "mama chandavasānugā”.",
  "ja534:82.1_0": "“Kacci raṭṭhaṁ anuppīḷaṁ,",
  "ja534:82.2_0": "akutociupaddavaṁ;",
  "ja534:82.3_0": "Asāhasena dhammena,",
  "ja534:82.4_0": "samena manusāsasi”.",
  "ja534:83.1_0": "“Atho raṭṭhaṁ anuppīḷaṁ,",
  "ja534:83.2_0": "akutociupaddavaṁ;",
  "ja534:83.3_0": "Asāhasena dhammena,",
  "ja534:83.4_0": "samena manusāsahaṁ”.",
  "ja534:84.1_0": "“Kacci santo apacitā,",
  "ja534:84.2_0": "Asanto parivajjitā;",
  "ja534:84.3_0": "No ce dhammaṁ niraṅkatvā,",
  "ja534:84.4_0": "Adhammamanuvattasi”.",
  "ja534:85.1_0": "“Santo ca me apacitā,",
  "ja534:85.2_0": "asanto parivajjitā;",
  "ja534:85.3_0": "Dhammamevānuvattāmi,",
  "ja534:85.4_0": "adhammo me niraṅkato”.",
  "ja534:86.1_0": "“Kacci nānāgataṁ dīghaṁ,",
  "ja534:86.2_0": "samavekkhasi khattiya;",
  "ja534:86.3_0": "Kacci matto madanīye,",
  "ja534:86.4_0": "paralokaṁ na santasi”.",
  "ja534:87.1_0": "“Nāhaṁ anāgataṁ dīghaṁ,",
  "ja534:87.2_0": "samavekkhāmi pakkhima;",
  "ja534:87.3_0": "Ṭhito dasasu dhammesu,",
  "ja534:87.4_0": "paralokaṁ na santase.",
  "ja534:88.1_0": "Dānaṁ sīlaṁ pariccāgaṁ,",
  "ja534:88.2_0": "ajjavaṁ maddavaṁ tapaṁ;",
  "ja534:88.3_0": "Akkodhaṁ avihiṁsañca,",
  "ja534:88.4_0": "khantiñca avirodhanaṁ.",
  "ja534:89.1_0": "Iccete kusale dhamme,",
  "ja534:89.2_0": "ṭhite passāmi attani;",
  "ja534:89.3_0": "Tato me jāyate pīti,",
  "ja534:89.4_0": "somanassañcanappakaṁ.",
  "ja534:90.1_0": "Sumukho ca acintetvā,",
  "ja534:90.2_0": "visajji pharusaṁ giraṁ;",
  "ja534:90.3_0": "Bhāvadosamanaññāya,",
  "ja534:90.4_0": "asmākāyaṁ vihaṅgamo.",
  "ja534:91.1_0": "So kuddho pharusaṁ vācaṁ,",
  "ja534:91.2_0": "nicchāresi ayoniso;",
  "ja534:91.3_0": "Yānasmesu na vijjanti,",
  "ja534:91.4_0": "nayidaṁ paññavatāmiva”.",
  "ja534:92.1_0": "“Atthi me taṁ atisāraṁ,",
  "ja534:92.2_0": "vegena manujādhipa;",
  "ja534:92.3_0": "Dhataraṭṭhe ca baddhasmiṁ,",
  "ja534:92.4_0": "dukkhaṁ me vipulaṁ ahu.",
  "ja534:93.1_0": "Tvaṁ no pitāva puttānaṁ,",
  "ja534:93.2_0": "bhūtānaṁ dharaṇīriva;",
  "ja534:93.3_0": "Asmākaṁ adhipannānaṁ,",
  "ja534:93.4_0": "khamassu rājakuñjara”.",
  "ja534:94.1_0": "“Etaṁ te anumodāma,",
  "ja534:94.2_0": "yaṁ bhāvaṁ na nigūhasi;",
  "ja534:94.3_0": "Khilaṁ pabhindasi pakkhi,",
  "ja534:94.4_0": "ujukosi vihaṅgama.",
  "ja534:95.1_0": "Yaṁ kiñci ratanaṁ atthi,",
  "ja534:95.2_0": "kāsirājanivesane;",
  "ja534:95.3_0": "Rajataṁ jātarūpañca,",
  "ja534:95.4_0": "muttā veḷuriyā bahū.",
  "ja534:96.1_0": "Maṇayo saṅkhamuttā ca,",
  "ja534:96.2_0": "vatthakaṁ haricandanaṁ;",
  "ja534:96.3_0": "Ajinaṁ dantabhaṇḍañca,",
  "ja534:96.4_0": "lohaṁ kāḷāyasaṁ bahuṁ;",
  "ja534:96.5_0": "Etaṁ dadāmi vo vittaṁ,",
  "ja534:96.6_0": "issaraṁ vissajāmi vo”.",
  "ja534:97.1_0": "“Addhā apacitā tyamhā,",
  "ja534:97.2_0": "sakkatā ca rathesabha;",
  "ja534:97.3_0": "Dhammesu vattamānānaṁ,",
  "ja534:97.4_0": "tvaṁ no ācariyo bhava.",
  "ja534:98.1_0": "Ācariya samanuññātā,",
  "ja534:98.2_0": "Tayā anumatā mayaṁ;",
  "ja534:98.3_0": "Taṁ padakkhiṇato katvā,",
  "ja534:98.4_0": "Ñātiṁ passemurindama”.",
  "ja534:99.1_0": "Sabbarattiṁ cintayitvā,",
  "ja534:99.2_0": "mantayitvā yathātathaṁ;",
  "ja534:99.3_0": "Kāsirājā anuññāsi,",
  "ja534:99.4_0": "haṁsānaṁ pavaruttamaṁ.",
  "ja534:100.1_0": "Tato ratyā vivasāne,",
  "ja534:100.2_0": "sūriyuggamanaṁ pati;",
  "ja534:100.3_0": "Pekkhato kāsirājassa,",
  "ja534:100.4_0": "bhavanā te vigāhisuṁ.",
  "ja534:101.1_0": "Te aroge anuppatte,",
  "ja534:101.2_0": "disvāna parame dije;",
  "ja534:101.3_0": "Kekāti makaruṁ haṁsā,",
  "ja534:101.4_0": "puthusaddo ajāyatha.",
  "ja534:102.1_0": "Te patītā pamuttena,",
  "ja534:102.2_0": "bhattunā bhattugāravā;",
  "ja534:102.3_0": "Samantā parikiriṁsu,",
  "ja534:102.4_0": "aṇḍajā laddhapaccayā.",
  "ja534:103.1_0": "Evaṁ mittavataṁ atthā,",
  "ja534:103.2_0": "sabbe honti padakkhiṇā;",
  "ja534:103.3_0": "Haṁsā yathā dhataraṭṭhā,",
  "ja534:103.4_0": "ñātisaṅghaṁ upāgamunti.",
  "ja534:104.1_0": "Mahāhaṁsajātakaṁ dutiyaṁ."
}