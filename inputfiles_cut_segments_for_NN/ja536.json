{
  "ja536:0.1_0": "Jātaka",
  "ja536:0.2_0": "Asītinipāta",
  "ja536:0.3_0": "Cūḷahaṁsavagga",
  "ja536:0.4_0": "4. Kuṇālajātaka",
  "ja536:1.1_0": "Evamakkhāyati, evamanusūyati.",
  "ja536:1.2_0": "Sabbosadhadharaṇidhare nekapupphamālyavitate gajagavajamahiṁsarurucamarapasadakhaggagokaṇṇasīhabyagghadīpiacchakokataracchauddārakadalimigabiḷārasasakaṇṇikānucarite ākiṇṇanelamaṇḍalamahāvarāhanāgakulakareṇusaṅghādhivuṭṭhe issamigasākhamigasarabhamigaeṇīmigavātamigapasadamigapurisālukimpurisayakkharakkhasanisevite amajjavamañjarīdharapahaṭṭhapupphaphusitaggānekapādapagaṇavitate kuraracakoravāraṇamayūraparabhatajīvañjīvakacelāvakābhiṅkārakaravīkamattavihaṅgagaṇasatatasampaghuṭṭhe añjanamanosilāharitālahiṅgulakahemarajatakanakānekadhātusatavinaddhapaṭimaṇḍitappadese evarūpe khalu, bho,",
  "ja536:1.2_1": "ramme vanasaṇḍe kuṇālo nāma sakuṇo paṭivasati ativiya citto ativiya cittapattacchadano.",
  "ja536:2.1_0": "Tasseva khalu, bho,",
  "ja536:2.1_1": "kuṇālassa sakuṇassa aḍḍhuḍḍhāni itthisahassāni paricārikā dijakaññāyo, atha khalu, bho,",
  "ja536:2.1_2": "dve dijakaññāyo kaṭṭhaṁ mukhena ḍaṁsitvā taṁ kuṇālaṁ sakuṇaṁ majjhe nisīdāpetvā uḍḍenti—",
  "ja536:2.2_0": "“mā naṁ kuṇālaṁ sakuṇaṁ addhānapariyāyapathe kilamatho ubbāhetthā”ti.",
  "ja536:3.1_0": "Pañcasatā dijakaññāyo heṭṭhato heṭṭhato uḍḍenti—",
  "ja536:3.2_0": "“sacāyaṁ kuṇālo sakuṇo āsanā paripatissati,",
  "ja536:3.2_1": "mayaṁ taṁ pakkhehi paṭiggahessāmā”ti.",
  "ja536:4.1_0": "Pañcasatā dijakaññāyo uparūpari uḍḍenti—",
  "ja536:4.2_0": "“mā naṁ kuṇālaṁ sakuṇaṁ ātapo paritāpesī”ti.",
  "ja536:5.1_0": "Pañcasatā pañcasatā dijakaññāyo ubhatopassena uḍḍenti—",
  "ja536:5.2_0": "“mā naṁ kuṇālaṁ sakuṇaṁ sītaṁ vā uṇhaṁ vā tiṇaṁ vā rajo vā vāto vā ussāvo vā upapphusī”ti.",
  "ja536:6.1_0": "Pañcasatā dijakaññāyo purato purato uḍḍenti—",
  "ja536:6.2_0": "“mā naṁ kuṇālaṁ sakuṇaṁ gopālakā vā pasupālakā vā tiṇahārakā vā kaṭṭhahārakā vā vanakammikā vā kaṭṭhena vā kaṭhalena vā pāṇinā vā (…) leḍḍunā vā daṇḍena vā satthena vā sakkharāhi vā pahāraṁ adaṁsu.",
  "ja536:6.3_0": "Māyaṁ kuṇālo sakuṇo gacchehi vā latāhi vā rukkhehi vā sākhāhi vā thambhehi vā pāsāṇehi vā balavantehi vā pakkhīhi saṅgamesī”ti.",
  "ja536:7.1_0": "Pañcasatā dijakaññāyo pacchato pacchato uḍḍenti saṇhāhi sakhilāhi mañjūhi madhurāhi vācāhi samudācarantiyo—",
  "ja536:7.2_0": "“māyaṁ kuṇālo sakuṇo āsane pariyukkaṇṭhī”ti.",
  "ja536:8.1_0": "Pañcasatā dijakaññāyo disodisaṁ uḍḍenti anekarukkhavividhavikatiphalamāharantiyo—",
  "ja536:8.2_0": "“māyaṁ kuṇālo sakuṇo khudāya parikilamitthā”ti.",
  "ja536:9.1_0": "Atha khalu, bho,",
  "ja536:9.1_1": "tā dijakaññāyo taṁ kuṇālaṁ sakuṇaṁ ārāmeneva ārāmaṁ uyyāneneva uyyānaṁ nadītittheneva nadītitthaṁ pabbatasikhareneva pabbatasikharaṁ ambavaneneva ambavanaṁ jambuvaneneva jambuvanaṁ labujavaneneva labujavanaṁ nāḷikerasañcāriyeneva nāḷikerasañcāriyaṁ khippameva abhisambhonti ratitthāya.",
  "ja536:10.1_0": "Atha khalu, bho,",
  "ja536:10.1_1": "kuṇālo sakuṇo tāhi dijakaññāhi divasaṁ paribyūḷho evaṁ apasādeti—",
  "ja536:10.2_0": "“nassatha tumhe vasaliyo, vinassatha tumhe vasaliyo,",
  "ja536:10.2_1": "coriyo dhuttiyo asatiyo lahucittāyo katassa appaṭikārikāyo anilo viya yenakāmaṅgamāyo”ti.",
  "ja536:11.1_0": "Tasseva khalu, bho,",
  "ja536:11.1_1": "himavato pabbatarājassa puratthimadisābhāge susukhumasunipuṇagirippabhava—",
  "ja536:11.2_0": "haritupayantiyo.",
  "ja536:12.1_0": "Uppala paduma kumuda nalina satapatta sogandhika mandālaka sampativirūḷhasucigandhamanuññamāvakappadese.",
  "ja536:13.1_0": "Kuravakamucalindaketakavedisavañjulapunnāgabakulatilakapiyakahasanasālasaḷalacampakaasokanāgarukkhatirīṭibhujapattaloddacandanoghavane kāḷāgarupadmakapiyaṅgudevadārukacocagahane kakudhakuṭajaaṅkolakaccikārakaṇikārakaṇṇikārakanaverakoraṇḍakakoviḷārakiṁsukayodhikavanamallikamanaṅgaṇamanavajjabhaṇḍisurucirabhaginimālāmalyadhare jātisumanamadhugandhikadhanutakkāritālīsatagaramusīrakoṭṭhakacchavitate atimuttakasaṅkusumitalatāvitatapaṭimaṇḍitappadese haṁsapilavakādambakāraṇḍavābhinadite vijjādharasiddhasamaṇatāpasagaṇādhivuṭṭhe varadevayakkharakkhasadānavagandhabbakinnaramahoragānuciṇṇappadese evarūpe khalu, bho,",
  "ja536:13.1_1": "ramme vanasaṇḍe puṇṇamukho nāma phussakokilo paṭivasati ativiya madhuragiro vilāsitanayano mattakkho.",
  "ja536:14.1_0": "Tasseva khalu, bho,",
  "ja536:14.1_1": "puṇṇamukhassa phussakokilassa aḍḍhuḍḍhāni itthisatāni paricārikā dijakaññāyo.",
  "ja536:14.2_0": "Atha khalu, bho,",
  "ja536:14.2_1": "dve dijakaññāyo kaṭṭhaṁ mukhena ḍaṁsitvā taṁ puṇṇamukhaṁ phussakokilaṁ majjhe nisīdāpetvā uḍḍenti—",
  "ja536:14.3_0": "“mā naṁ puṇṇamukhaṁ phussakokilaṁ addhānapariyāyapathe kilamatho ubbāhetthā”ti.",
  "ja536:15.1_0": "Paññāsa dijakaññāyo heṭṭhato heṭṭhato uḍḍenti—",
  "ja536:15.2_0": "“sacāyaṁ puṇṇamukho phussakokilo āsanā paripatissati,",
  "ja536:15.2_1": "mayaṁ taṁ pakkhehi paṭiggahessāmā”ti.",
  "ja536:16.1_0": "Paññāsa dijakaññāyo uparūpari uḍḍenti—",
  "ja536:16.2_0": "“mā naṁ puṇṇamukhaṁ phussakokilaṁ ātapo paritāpesī”ti.",
  "ja536:17.1_0": "Paññāsa paññāsa dijakaññāyo ubhatopassena uḍḍenti—",
  "ja536:17.2_0": "“mā naṁ puṇṇamukhaṁ phussakokilaṁ sītaṁ vā uṇhaṁ vā tiṇaṁ vā rajo vā vāto vā ussāvo vā upapphusī”ti.",
  "ja536:18.1_0": "Paññāsa dijakaññāyo purato purato uḍḍenti—",
  "ja536:18.2_0": "“mā naṁ puṇṇamukhaṁ phussakokilaṁ gopālakā vā pasupālakā vā tiṇahārakā vā kaṭṭhahārakā vā vanakammikā vā kaṭṭhena vā kaṭhalena vā pāṇinā vā leḍḍunā vā daṇḍena vā satthena vā sakkharāhi vā pahāraṁ adaṁsu.",
  "ja536:18.3_0": "Māyaṁ puṇṇamukho phussakokilo gacchehi vā latāhi vā rukkhehi vā sākhāhi vā thambhehi vā pāsāṇehi vā balavantehi vā pakkhīhi saṅgamesī”ti.",
  "ja536:19.1_0": "Paññāsa dijakaññāyo pacchato pacchato uḍḍenti saṇhāhi sakhilāhi mañjūhi madhurāhi vācāhi samudācarantiyo—",
  "ja536:19.2_0": "“māyaṁ puṇṇamukho phussakokilo āsane pariyukkaṇṭhī”ti.",
  "ja536:20.1_0": "Paññāsa dijakaññāyo disodisaṁ uḍḍenti anekarukkhavividhavikatiphalamāharantiyo—",
  "ja536:20.2_0": "“māyaṁ puṇṇamukho phussakokilo khudāya parikilamitthā”ti.",
  "ja536:21.1_0": "Atha khalu, bho,",
  "ja536:21.1_1": "tā dijakaññāyo taṁ puṇṇamukhaṁ phussakokilaṁ ārāmeneva ārāmaṁ uyyāneneva uyyānaṁ nadītittheneva nadītitthaṁ pabbatasikhareneva pabbatasikharaṁ ambavaneneva ambavanaṁ jambuvaneneva jambuvanaṁ labujavaneneva labujavanaṁ nāḷikerasañcāriyeneva nāḷikerasañcāriyaṁ khippameva abhisambhonti ratitthāya.",
  "ja536:22.1_0": "Atha khalu, bho,",
  "ja536:22.1_1": "puṇṇamukho phussakokilo tāhi dijakaññāhi divasaṁ paribyūḷho evaṁ pasaṁsati—",
  "ja536:22.2_0": "“sādhu sādhu, bhaginiyo, etaṁ kho, bhaginiyo,",
  "ja536:22.2_1": "tumhākaṁ patirūpaṁ kuladhītānaṁ,",
  "ja536:22.2_2": "yaṁ tumhe bhattāraṁ paricareyyāthā”ti.",
  "ja536:23.1_0": "Atha khalu, bho,",
  "ja536:23.1_1": "puṇṇamukho phussakokilo yena kuṇālo sakuṇo tenupasaṅkami.",
  "ja536:23.2_0": "Addasaṁsu kho kuṇālassa sakuṇassa paricārikā dijakaññāyo taṁ puṇṇamukhaṁ phussakokilaṁ dūratova āgacchantaṁ;",
  "ja536:23.3_0": "disvāna yena puṇṇamukho phussakokilo tenupasaṅkamiṁsu;",
  "ja536:23.3_1": "upasaṅkamitvā taṁ puṇṇamukhaṁ phussakokilaṁ etadavocuṁ—",
  "ja536:23.4_0": "“ayaṁ, samma puṇṇamukha,",
  "ja536:23.4_1": "kuṇālo sakuṇo ativiya pharuso ativiya pharusavāco,",
  "ja536:23.4_2": "appevanāma tavampi āgamma piyavācaṁ labheyyāmā”ti.",
  "ja536:23.5_0": "“Appevanāma,",
  "ja536:23.5_1": "bhaginiyo”ti vatvā yena kuṇālo sakuṇo tenupasaṅkami;",
  "ja536:23.5_2": "upasaṅkamitvā kuṇālena sakuṇena saddhiṁ paṭisammoditvā ekamantaṁ nisīdi.",
  "ja536:23.6_0": "Ekamantaṁ nisinno kho puṇṇamukho phussakokilo taṁ kuṇālaṁ sakuṇaṁ etadavoca—",
  "ja536:23.7_0": "“kissa tvaṁ, samma kuṇāla,",
  "ja536:23.7_1": "itthīnaṁ sujātānaṁ kuladhītānaṁ sammāpaṭipannānaṁ micchāpaṭipannosi?",
  "ja536:23.8_0": "Amanāpabhāṇīnampi kira, samma kuṇāla,",
  "ja536:23.8_1": "itthīnaṁ manāpabhāṇinā bhavitabbaṁ,",
  "ja536:23.8_2": "kimaṅga pana manāpabhāṇīnan”ti.",
  "ja536:24.1_0": "Evaṁ vutte,",
  "ja536:24.1_1": "kuṇālo sakuṇo taṁ puṇṇamukhaṁ phussakokilaṁ evaṁ apasādesi—",
  "ja536:24.2_0": "“nassa tvaṁ, samma jamma vasala, vinassa tvaṁ, samma jamma vasala,",
  "ja536:24.2_1": "ko nu tayā viyatto jāyājinenā”ti.",
  "ja536:24.3_0": "Evaṁ apasādito ca pana puṇṇamukho phussakokilo tatoyeva paṭinivatti.",
  "ja536:25.1_0": "Atha khalu, bho,",
  "ja536:25.1_1": "puṇṇamukhassa phussakokilassa aparena samayena nacirasseva kharo ābādho uppajji lohitapakkhandikā bāḷhā vedanā vattanti māraṇantikā.",
  "ja536:25.2_0": "Atha khalu, bho,",
  "ja536:25.2_1": "puṇṇamukhassa phussakokilassa paricārikānaṁ dijakaññānaṁ etadahosi—",
  "ja536:25.3_0": "“ābādhiko kho ayaṁ puṇṇamukho phussakokilo,",
  "ja536:25.3_1": "appevanāma imamhā ābādhā vuṭṭhaheyyā”ti ekaṁ adutiyaṁ ohāya yena kuṇālo sakuṇo tenupasaṅkamiṁsu.",
  "ja536:25.4_0": "Addasā kho kuṇālo sakuṇo tā dijakaññāyo dūratova āgacchantiyo,",
  "ja536:25.4_1": "disvāna tā dijakaññāyo etadavoca—",
  "ja536:25.5_0": "“kahaṁ pana tumhaṁ vasaliyo bhattā”ti?",
  "ja536:25.6_0": "“Ābādhiko kho, samma kuṇāla,",
  "ja536:25.6_1": "puṇṇamukho phussakokilo appevanāma tamhā ābādhā vuṭṭhaheyyā”ti.",
  "ja536:25.7_0": "Evaṁ vutte,",
  "ja536:25.7_1": "kuṇālo sakuṇo tā dijakaññāyo evaṁ apasādesi—",
  "ja536:25.8_0": "“nassatha tumhe vasaliyo, vinassatha tumhe vasaliyo,",
  "ja536:25.8_1": "coriyo dhuttiyo asatiyo lahucittāyo katassa appaṭikārikāyo anilo viya yenakāmaṅgamāyo”ti;",
  "ja536:25.9_0": "vatvā yena puṇṇamukho phussakokilo tenupasaṅkami;",
  "ja536:25.9_1": "upasaṅkamitvā taṁ puṇṇamukhaṁ phussakokilaṁ etadavoca—",
  "ja536:25.10_0": "“haṁ, samma, puṇṇamukhā”ti.",
  "ja536:25.11_0": "“Haṁ, samma, kuṇālā”ti.",
  "ja536:26.1_0": "Atha khalu,",
  "ja536:26.1_1": "bho kuṇālo sakuṇo taṁ puṇṇamukhaṁ phussakokilaṁ pakkhehi ca mukhatuṇḍakena ca pariggahetvā vuṭṭhāpetvā nānābhesajjāni pāyāpesi.",
  "ja536:26.2_0": "Atha khalu, bho,",
  "ja536:26.2_1": "puṇṇamukhassa phussakokilassa so ābādho paṭippassambhīti.",
  "ja536:26.3_0": "Atha khalu, bho,",
  "ja536:26.3_1": "kuṇālo sakuṇo taṁ puṇṇamukhaṁ phussakokilaṁ gilānavuṭṭhitaṁ aciravuṭṭhitaṁ gelaññā etadavoca—",
  "ja536:27.1_0": "“Diṭṭhā mayā, samma puṇṇamukha,",
  "ja536:27.1_1": "kaṇhā dvepitikā pañcapatikāya chaṭṭhe purise cittaṁ paṭibandhantiyā,",
  "ja536:27.1_2": "yadidaṁ kabandhe pīṭhasappimhī”ti.",
  "ja536:27.2_0": "Bhavati ca panuttarettha vākyaṁ—",
  "ja536:28.1_0": "“Athajjuno nakulo bhīmaseno,",
  "ja536:28.2_0": "Yudhiṭṭhilo sahadevo ca rājā;",
  "ja536:28.3_0": "Ete patī pañca maticca nārī,",
  "ja536:28.4_0": "Akāsi khujjavāmanakena pāpanti.",
  "ja536:29.1_0": "Diṭṭhā mayā, samma puṇṇamukha,",
  "ja536:29.1_1": "saccatapāpī nāma samaṇī susānamajjhe vasantī catutthabhattaṁ pariṇāmayamānā surādhuttakena pāpamakāsi.",
  "ja536:30.1_0": "Diṭṭhā mayā, samma puṇṇamukha,",
  "ja536:30.1_1": "kākavatī nāma devī samuddamajjhe vasantī bhariyā venateyyassa naṭakuverena pāpamakāsi.",
  "ja536:31.1_0": "Diṭṭhā mayā, samma puṇṇamukha,",
  "ja536:31.1_1": "kuruṅgadevī nāma lomasuddarī eḷikakumāraṁ kāmayamānā chaḷaṅgakumāradhanantevāsinā pāpamakāsi.",
  "ja536:32.1_0": "Evañhetaṁ mayā ñātaṁ,",
  "ja536:32.2_0": "Brahmadattassa mātaraṁ;",
  "ja536:32.3_0": "Ohāya kosalarājaṁ,",
  "ja536:32.4_0": "Pañcālacaṇḍena pāpamakāsi.",
  "ja536:33.1_0": "Etā ca aññā ca akaṁsu pāpaṁ,",
  "ja536:33.2_0": "Tasmāhamitthīnaṁ na vissase nappasaṁse;",
  "ja536:33.3_0": "Mahī yathā jagati samānarattā,",
  "ja536:33.4_0": "Vasundharā itarītarā patiṭṭhā;",
  "ja536:33.5_0": "Sabbasahā aphandanā akuppā,",
  "ja536:33.6_0": "Tathitthiyo tāyo na vissase naro.",
  "ja536:34.1_0": "Sīho yathā lohitamaṁsabhojano,",
  "ja536:34.2_0": "Vāḷamigo pañcāvudho suruddho;",
  "ja536:34.3_0": "Pasayhakhādī parahiṁsane rato,",
  "ja536:34.4_0": "Tathitthiyo tāyo na vissase naro.",
  "ja536:35.1_0": "Na khalu, samma puṇṇamukha, vesiyo nāriyo gamaniyo, na hetā bandhakiyo nāma, vadhikāyo nāma etāyo,",
  "ja536:35.1_1": "yadidaṁ vesiyo nāriyo gamaniyoti.",
  "ja536:36.1_0": "Coro viya veṇikatā,",
  "ja536:36.1_1": "madirāva diddhā vāṇijo viya vācāsanthutiyo,",
  "ja536:36.1_2": "issasiṅghamiva viparivattāyo, uragāmiva dujivhāyo, sobbhamiva paṭicchannā,",
  "ja536:36.1_3": "pātālamiva duppūrā rakkhasī viya duttosā, yamovekantahāriyo, sikhīriva sabbabhakkhā, nadīriva sabbavāhī, anilo viya yenakāmaṁcarā, neru viya avisesakarā,",
  "ja536:36.1_4": "visarukkho viya niccaphalitāyoti.",
  "ja536:36.2_0": "Bhavati ca panuttarettha vākyaṁ—",
  "ja536:37.1_0": "Yathā coro yathā diddho,",
  "ja536:37.2_0": "Vāṇijova vikatthanī;",
  "ja536:37.3_0": "Issasiṅghamiva parivattā,",
  "ja536:37.4_0": "Dujivhā urago viya.",
  "ja536:38.1_0": "Sobbhamiva paṭicchannā,",
  "ja536:38.2_0": "pātālamiva duppurā;",
  "ja536:38.3_0": "Rakkhasī viya duttosā,",
  "ja536:38.4_0": "yamovekantahāriyo.",
  "ja536:39.1_0": "Yathā sikhī nadī vāto,",
  "ja536:39.2_0": "Nerunāva samāgatā;",
  "ja536:39.3_0": "Visarukkho viya niccaphalā,",
  "ja536:39.4_0": "Nāsayanti ghare bhogaṁ;",
  "ja536:39.5_0": "Ratanantakaritthiyoti.",
  "ja536:40.1_0": "Cattārimāni, samma puṇṇamukha,",
  "ja536:40.1_1": "yāni vatthūni kicce jāte anatthacarāni bhavanti;",
  "ja536:40.2_0": "tāni parakule na vāsetabbāni—",
  "ja536:40.3_0": "goṇaṁ dhenuṁ yānaṁ bhariyā.",
  "ja536:40.4_0": "Cattāri etāni paṇḍito dhanāni gharā na vippavāsaye.",
  "ja536:41.1_0": "Goṇaṁ dhenuñca yānañca,",
  "ja536:41.2_0": "Bhariyaṁ ñātikule na vāsaye;",
  "ja536:41.3_0": "Bhañjanti rathaṁ ayānakā,",
  "ja536:41.4_0": "Ativāhena hananti puṅgavaṁ;",
  "ja536:41.5_0": "Dohena hananti vacchakaṁ,",
  "ja536:41.6_0": "Bhariyā ñātikule padussatīti.",
  "ja536:42.1_0": "Cha imāni, samma puṇṇamukha,",
  "ja536:42.1_1": "yāni vatthūni kicce jāte anatthacarāni bhavanti—",
  "ja536:43.1_0": "Aguṇaṁ dhanu ñātikule ca bhariyā,",
  "ja536:43.2_0": "Pāraṁ nāvā akkhabhaggañca yānaṁ;",
  "ja536:43.3_0": "Dūre mitto pāpasahāyako ca,",
  "ja536:43.4_0": "Kicce jāte anatthacarāni bhavanti.",
  "ja536:44.1_0": "Aṭṭhahi khalu, samma puṇṇamukha,",
  "ja536:44.1_1": "ṭhānehi itthī sāmikaṁ avajānāti.",
  "ja536:44.2_0": "Daliddatā, āturatā, jiṇṇatā, surāsoṇḍatā, muddhatā, pamattatā, sabbakiccesu anuvattanatā, sabbadhanaanuppadānena—",
  "ja536:44.3_0": "imehi khalu, samma puṇṇamukha,",
  "ja536:44.3_1": "aṭṭhahi ṭhānehi itthī sāmikaṁ avajānāti.",
  "ja536:44.4_0": "Bhavati ca panuttarettha vākyaṁ—",
  "ja536:45.1_0": "Daliddaṁ āturañcāpi,",
  "ja536:45.2_0": "jiṇṇakaṁ surasoṇḍakaṁ;",
  "ja536:45.3_0": "Pamattaṁ muddhapattañca,",
  "ja536:45.4_0": "sabbakiccesu hāpanaṁ;",
  "ja536:45.5_0": "Sabbakāmappadānena,",
  "ja536:45.6_0": "avajānāti sāmikanti.",
  "ja536:46.1_0": "Navahi khalu, samma puṇṇamukha, ṭhānehi itthī padosamāharati.",
  "ja536:46.2_0": "Ārāmagamanasīlā ca hoti, uyyānagamanasīlā ca hoti,",
  "ja536:46.2_1": "nadītitthagamanasīlā ca hoti, ñātikulagamanasīlā ca hoti, parakulagamanasīlā ca hoti,",
  "ja536:46.2_2": "ādāsadussamaṇḍanānuyogamanuyuttasīlā ca hoti, majjapāyinī ca hoti, nillokanasīlā ca hoti, sadvāraṭhāyinī ca hoti—",
  "ja536:46.3_0": "imehi khalu, samma puṇṇamukha,",
  "ja536:46.3_1": "navahi ṭhānehi itthī padosamāharatīti.",
  "ja536:46.4_0": "Bhavati ca panuttarettha vākyaṁ—",
  "ja536:47.1_0": "Ārāmasīlā ca uyyānaṁ,",
  "ja536:47.2_0": "Nadī ñāti parakulaṁ;",
  "ja536:47.3_0": "Ādāsadussamaṇḍanamanuyuttā,",
  "ja536:47.4_0": "Yā citthī majjapāyinī.",
  "ja536:48.1_0": "Yā ca nillokanasīlā,",
  "ja536:48.2_0": "Yā ca sadvāraṭhāyinī;",
  "ja536:48.3_0": "Navahetehi ṭhānehi,",
  "ja536:48.4_0": "Padosamāharanti itthiyoti.",
  "ja536:49.1_0": "Cattālīsāya khalu, samma puṇṇamukha,",
  "ja536:49.1_1": "ṭhānehi itthī purisaṁ accācarati.",
  "ja536:49.2_0": "Vijambhati, vinamati, vilasati, vilajjati, nakhena nakhaṁ ghaṭṭeti, pādena pādaṁ akkamati, kaṭṭhena pathaviṁ vilikhati,",
  "ja536:49.2_1": "dārakaṁ ullaṅghati ullaṅghāpeti, kīḷati kīḷāpeti, cumbati cumbāpeti, bhuñjati bhuñjāpeti, dadāti, yācati, katamanukaroti, uccaṁ bhāsati, nīcaṁ bhāsati, aviccaṁ bhāsati, viviccaṁ bhāsati,",
  "ja536:49.2_2": "naccena gītena vāditena rodanena vilasitena vibhūsitena jagghati, pekkhati, kaṭiṁ cāleti, guyhabhaṇḍakaṁ sañcāleti, ūruṁ vivarati, ūruṁ pidahati, thanaṁ dasseti, kacchaṁ dasseti, nābhiṁ dasseti, akkhiṁ nikhanati, bhamukaṁ ukkhipati, oṭṭhaṁ upalikhati, jivhaṁ nillāleti, dussaṁ muñcati, dussaṁ paṭibandhati, sirasaṁ muñcati, sirasaṁ bandhati—",
  "ja536:49.3_0": "imehi khalu, samma puṇṇamukha,",
  "ja536:49.3_1": "cattālīsāya ṭhānehi itthī purisaṁ accācarati.",
  "ja536:50.1_0": "Pañcavīsāya khalu, samma puṇṇamukha,",
  "ja536:50.1_1": "ṭhānehi itthī paduṭṭhā veditabbā bhavati.",
  "ja536:50.2_0": "Sāmikassa pavāsaṁ vaṇṇeti, pavuṭṭhaṁ na sarati, āgataṁ nābhinandati, avaṇṇaṁ tassa bhaṇati, vaṇṇaṁ tassa na bhaṇati, anatthaṁ tassa carati, atthaṁ tassa na carati, akiccaṁ tassa karoti, kiccaṁ tassa na karoti, paridahitvā sayati, parammukhī nipajjati,",
  "ja536:50.2_1": "parivattakajātā kho pana hoti kuṅkumiyajātā, dīghaṁ assasati, dukkhaṁ vedayati,",
  "ja536:50.2_2": "uccārapassāvaṁ abhiṇhaṁ gacchati, vilomamācarati,",
  "ja536:50.2_3": "parapurisasaddaṁ sutvā kaṇṇasotaṁ vivaramodahati, nihatabhogā kho pana hoti,",
  "ja536:50.2_4": "paṭivissakehi santhavaṁ karoti, nikkhantapādā kho pana hoti,",
  "ja536:50.2_5": "visikhānucārinī aticārinī kho pana hoti,",
  "ja536:50.2_6": "niccaṁ sāmike agāravā paduṭṭhamanasaṅkappā, abhiṇhaṁ dvāre tiṭṭhati,",
  "ja536:50.2_7": "kacchāni aṅgāni thanāni dasseti, disodisaṁ gantvā pekkhati—",
  "ja536:50.3_0": "imehi khalu, samma puṇṇamukha,",
  "ja536:50.3_1": "pañcavīsāya ṭhānehi itthī paduṭṭhā veditabbā bhavati.",
  "ja536:50.4_0": "Bhavati ca panuttarettha vākyaṁ—",
  "ja536:51.1_0": "Pavāsaṁ tassa vaṇṇeti,",
  "ja536:51.2_0": "Gataṁ tassa na socati;",
  "ja536:51.3_0": "Disvāna patimāgataṁ nābhinandati,",
  "ja536:51.4_0": "Bhattāravaṇṇaṁ na kadāci bhāsati;",
  "ja536:51.5_0": "Ete paduṭṭhāya bhavanti lakkhaṇā.",
  "ja536:52.1_0": "Anatthaṁ tassa carati asaññatā,",
  "ja536:52.2_0": "Atthañca hāpeti akiccakārinī;",
  "ja536:52.3_0": "Paridahitvā sayati parammukhī,",
  "ja536:52.4_0": "Ete paduṭṭhāya bhavanti lakkhaṇā.",
  "ja536:53.1_0": "Parivattajātā ca bhavati kuṅkumī,",
  "ja536:53.2_0": "Dīghañca assasati dukkhavedinī;",
  "ja536:53.3_0": "Uccārapassāvamabhiṇhaṁ gacchati,",
  "ja536:53.4_0": "Ete paduṭṭhāya bhavanti lakkhaṇā.",
  "ja536:54.1_0": "Vilomamācarati akiccakārinī,",
  "ja536:54.2_0": "Saddaṁ nisāmeti parassa bhāsato;",
  "ja536:54.3_0": "Nihatabhogā ca karoti santhavaṁ,",
  "ja536:54.4_0": "Ete paduṭṭhāya bhavanti lakkhaṇā.",
  "ja536:55.1_0": "Kicchena laddhaṁ kasirābhataṁ dhanaṁ,",
  "ja536:55.2_0": "Vittaṁ vināseti dukkhena sambhataṁ;",
  "ja536:55.3_0": "Paṭivissakehi ca karoti santhavaṁ,",
  "ja536:55.4_0": "Ete paduṭṭhāya bhavanti lakkhaṇā.",
  "ja536:56.1_0": "Nikkhantapādā visikhānucārinī,",
  "ja536:56.2_0": "Niccañca sāmimhi paduṭṭhamānasā;",
  "ja536:56.3_0": "Aticārinī hoti apetagāravā,",
  "ja536:56.4_0": "Ete paduṭṭhāya bhavanti lakkhaṇā.",
  "ja536:57.1_0": "Abhikkhaṇaṁ tiṭṭhati dvāramūle,",
  "ja536:57.2_0": "Thanāni kacchāni ca dassayantī;",
  "ja536:57.3_0": "Disodisaṁ pekkhati bhantacittā,",
  "ja536:57.4_0": "Ete paduṭṭhāya bhavanti lakkhaṇā.",
  "ja536:58.1_0": "Sabbā nadī vaṅkagatī,",
  "ja536:58.2_0": "sabbe kaṭṭhamayā vanā;",
  "ja536:58.3_0": "Sabbitthiyo kare pāpaṁ,",
  "ja536:58.4_0": "labhamāne nivātake.",
  "ja536:59.1_0": "Sace labhetha khaṇaṁ vā raho vā,",
  "ja536:59.2_0": "Nivātakaṁ vāpi labhetha tādisaṁ;",
  "ja536:59.3_0": "Sabbāva itthī kayiruṁ nu pāpaṁ,",
  "ja536:59.4_0": "Aññaṁ alattha pīṭhasappināpi saddhiṁ.",
  "ja536:60.1_0": "Narānamārāmakarāsu nārisu,",
  "ja536:60.2_0": "Anekacittāsu aniggahāsu ca;",
  "ja536:60.3_0": "Sabbattha nāpītikarāpi ce siyā,",
  "ja536:60.4_0": "Na vissase titthasamā hi nāriyoti.",
  "ja536:61.1_0": "Yaṁ ve disvā kaṇḍarīkinnarānaṁ,",
  "ja536:61.2_0": "Sabbitthiyo na ramanti agāre;",
  "ja536:61.3_0": "Taṁ tādisaṁ maccaṁ cajitvā bhariyā,",
  "ja536:61.4_0": "Aññaṁ disvā purisaṁ pīṭhasappiṁ.",
  "ja536:62.1_0": "Bakassa ca bāvarikassa rañño,",
  "ja536:62.2_0": "Accantakāmānugatassa bhariyā;",
  "ja536:62.3_0": "Avācarī paṭṭhavasānugassa,",
  "ja536:62.4_0": "Kaṁ vāpi itthī nāticare tadaññaṁ.",
  "ja536:63.1_0": "Piṅgiyānī sabbalokissarassa,",
  "ja536:63.2_0": "Rañño piyā brahmadattassa bhariyā;",
  "ja536:63.3_0": "Avācarī paṭṭhavasānugassa,",
  "ja536:63.4_0": "Taṁ vāpi sā nājjhagā kāmakāminī.",
  "ja536:64.1_0": "Luddhānaṁ lahucittānaṁ,",
  "ja536:64.2_0": "Akataññūna dubbhinaṁ;",
  "ja536:64.3_0": "Nādevasatto puriso,",
  "ja536:64.4_0": "Thīnaṁ saddhātumarahati.",
  "ja536:65.1_0": "Na tā pajānanti kataṁ na kiccaṁ,",
  "ja536:65.2_0": "Na mātaraṁ pitaraṁ bhātaraṁ vā;",
  "ja536:65.3_0": "Anariyā samatikkantadhammā,",
  "ja536:65.4_0": "Sasseva cittassa vasaṁ vajanti.",
  "ja536:66.1_0": "Cirānuvuṭṭhampi piyaṁ manāpaṁ,",
  "ja536:66.2_0": "Anukampakaṁ pāṇasamampi bhattuṁ;",
  "ja536:66.3_0": "Āvāsu kiccesu ca naṁ jahanti,",
  "ja536:66.4_0": "Tasmāhamitthīnaṁ na vissasāmi.",
  "ja536:67.1_0": "Thīnañhi cittaṁ yathā vānarassa,",
  "ja536:67.2_0": "Kannappakannaṁ yathā rukkhachāyā;",
  "ja536:67.3_0": "Calācalaṁ hadayamitthiyānaṁ,",
  "ja536:67.4_0": "Cakkassa nemi viya parivattati.",
  "ja536:68.1_0": "Yadā tā passanti samekkhamānā,",
  "ja536:68.2_0": "Ādeyyarūpaṁ purisassa vittaṁ;",
  "ja536:68.3_0": "Saṇhāhi vācāhi nayanti menaṁ,",
  "ja536:68.4_0": "Kambojakā jalajeneva assaṁ.",
  "ja536:69.1_0": "Yadā na passanti samekkhamānā,",
  "ja536:69.2_0": "Ādeyyarūpaṁ purisassa vittaṁ;",
  "ja536:69.3_0": "Samantato naṁ parivajjayanti,",
  "ja536:69.4_0": "Tiṇṇo nadīpāragatova kullaṁ.",
  "ja536:70.1_0": "Silesūpamā sikhiriva sabbabhakkhā,",
  "ja536:70.2_0": "Tikkhamāyā nadīriva sīghasotā;",
  "ja536:70.3_0": "Sevanti hetā piyamappiyañca,",
  "ja536:70.4_0": "Nāvā yathā orakūlaṁ parañca.",
  "ja536:71.1_0": "Na tā ekassa na dvinnaṁ,",
  "ja536:71.2_0": "āpaṇova pasārito;",
  "ja536:71.3_0": "Yo tā mayhanti maññeyya,",
  "ja536:71.4_0": "vātaṁ jālena bādhaye.",
  "ja536:72.1_0": "Yathā nadī ca pantho ca,",
  "ja536:72.2_0": "pānāgāraṁ sabhā papā;",
  "ja536:72.3_0": "Evaṁ lokitthiyo nāma,",
  "ja536:72.4_0": "velā tāsaṁ na vijjati.",
  "ja536:73.1_0": "Ghatāsanasamā etā,",
  "ja536:73.2_0": "kaṇhasappasirūpamā;",
  "ja536:73.3_0": "Gāvo bahitiṇasseva,",
  "ja536:73.4_0": "omasanti varaṁ varaṁ.",
  "ja536:74.1_0": "Ghatāsanaṁ kuñjaraṁ kaṇhasappaṁ,",
  "ja536:74.2_0": "Muddhābhisittaṁ pamadā ca sabbā;",
  "ja536:74.3_0": "Ete naro niccayato bhajetha,",
  "ja536:74.4_0": "Tesaṁ have dubbidu sabbabhāvo.",
  "ja536:75.1_0": "Naccantavaṇṇā na bahūna kantā,",
  "ja536:75.2_0": "Na dakkhiṇā pamadā sevitabbā;",
  "ja536:75.3_0": "Na parassa bhariyā na dhanassa hetu,",
  "ja536:75.4_0": "Etitthiyo pañca na sevitabbā”.",
  "ja536:76.1_0": "Atha khalu, bho,",
  "ja536:76.1_1": "ānando gijjharājā kuṇālassa sakuṇassa ādimajjhakathāpariyosānaṁ viditvā tāyaṁ velāyaṁ imā gāthāyo abhāsi—",
  "ja536:77.1_0": "“Puṇṇampi cemaṁ pathaviṁ dhanena,",
  "ja536:77.2_0": "Dajjitthiyā puriso sammatāya;",
  "ja536:77.3_0": "Laddhā khaṇaṁ atimaññeyya tampi,",
  "ja536:77.4_0": "Tāsaṁ vasaṁ asatīnaṁ na gacche.",
  "ja536:78.1_0": "Uṭṭhāhakañcepi alīnavuttiṁ,",
  "ja536:78.2_0": "Komārabhattāraṁ piyaṁ manāpaṁ;",
  "ja536:78.3_0": "Āvāsu kiccesu ca naṁ jahanti,",
  "ja536:78.4_0": "Tasmāhamitthīnaṁ na vissasāmi.",
  "ja536:79.1_0": "Na vissase icchati manti poso,",
  "ja536:79.2_0": "Na vissase rodati me sakāse;",
  "ja536:79.3_0": "Sevanti hetā piyamappiyañca,",
  "ja536:79.4_0": "Nāvā yathā orakūlaṁ parañca.",
  "ja536:80.1_0": "Na vissase sākhapurāṇasanthataṁ,",
  "ja536:80.2_0": "Na vissase mittapurāṇacoraṁ;",
  "ja536:80.3_0": "Na vissase rājānaṁ sakhā mamanti,",
  "ja536:80.4_0": "Na vissase itthi dasanna mātaraṁ.",
  "ja536:81.1_0": "Na vissase rāmakarāsu nārisu,",
  "ja536:81.2_0": "Accantasīlāsu asaññatāsu;",
  "ja536:81.3_0": "Accantapemānugatassa bhariyā,",
  "ja536:81.4_0": "Na vissase titthasamā hi nāriyo.",
  "ja536:82.1_0": "Haneyyuṁ chindeyyuṁ chedāpeyyumpi,",
  "ja536:82.2_0": "Kaṇṭhepi chetvā rudhiraṁ piveyyuṁ;",
  "ja536:82.3_0": "Mā dīnakāmāsu asaññatāsu,",
  "ja536:82.4_0": "Bhāvaṁ kare gaṅgatitthūpamāsu.",
  "ja536:83.1_0": "Musā tāsaṁ yathā saccaṁ,",
  "ja536:83.2_0": "saccaṁ tāsaṁ yathā musā;",
  "ja536:83.3_0": "Gāvo bahitiṇasseva,",
  "ja536:83.4_0": "omasanti varaṁ varaṁ.",
  "ja536:84.1_0": "Gatenetā palobhenti,",
  "ja536:84.2_0": "pekkhitena mhitena ca;",
  "ja536:84.3_0": "Athopi dunnivatthena,",
  "ja536:84.4_0": "mañjunā bhaṇitena ca.",
  "ja536:85.1_0": "Coriyo kathinā hetā,",
  "ja536:85.2_0": "vāḷā ca lapasakkharā;",
  "ja536:85.3_0": "Na tā kiñci na jānanti,",
  "ja536:85.4_0": "yaṁ manussesu vañcanaṁ.",
  "ja536:86.1_0": "Asā lokitthiyo nāma,",
  "ja536:86.2_0": "velā tāsaṁ na vijjati;",
  "ja536:86.3_0": "Sārattā ca pagabbhā ca,",
  "ja536:86.4_0": "sikhī sabbaghaso yathā.",
  "ja536:87.1_0": "Natthitthīnaṁ piyo nāma,",
  "ja536:87.2_0": "Appiyopi na vijjati;",
  "ja536:87.3_0": "Sevanti hetā piyamappiyañca,",
  "ja536:87.4_0": "Nāvā yathā orakūlaṁ parañca.",
  "ja536:88.1_0": "Natthitthīnaṁ piyo nāma,",
  "ja536:88.2_0": "appiyopi na vijjati;",
  "ja536:88.3_0": "Dhanattā paṭivallanti,",
  "ja536:88.4_0": "latāva dumanissitā.",
  "ja536:89.1_0": "Hatthibandhaṁ assabandhaṁ,",
  "ja536:89.2_0": "Gopurisañca maṇḍalaṁ;",
  "ja536:89.3_0": "Chavaḍāhakaṁ pupphachaḍḍakaṁ,",
  "ja536:89.4_0": "Sadhanamanupatanti nāriyo.",
  "ja536:90.1_0": "Kulaputtampi jahanti akiñcanaṁ,",
  "ja536:90.2_0": "Chavakasamasadisampi;",
  "ja536:90.3_0": "Anugacchanti anupatanti,",
  "ja536:90.4_0": "Dhanahetu hi nāriyo”ti.",
  "ja536:91.1_0": "Atha khalu, bho,",
  "ja536:91.1_1": "nārado devabrāhmaṇo ānandassa gijjharājassa ādimajjhakathāpariyosānaṁ viditvā tāyaṁ velāyaṁ imā gāthāyo abhāsi—",
  "ja536:92.1_0": "“Cattārome na pūrenti,",
  "ja536:92.2_0": "te me suṇātha bhāsato;",
  "ja536:92.3_0": "Samuddo brāhmaṇo rājā,",
  "ja536:92.4_0": "itthī cāpi dijampati.",
  "ja536:93.1_0": "Saritā sāgaraṁ yanti,",
  "ja536:93.2_0": "yā kāci pathavissitā;",
  "ja536:93.3_0": "Tā samuddaṁ na pūrenti,",
  "ja536:93.4_0": "ūnattā hi na pūrati.",
  "ja536:94.1_0": "Brāhmaṇo ca adhīyāna,",
  "ja536:94.2_0": "vedamakkhānapañcamaṁ;",
  "ja536:94.3_0": "Bhiyyopi sutamiccheyya,",
  "ja536:94.4_0": "ūnattā hi na pūrati.",
  "ja536:95.1_0": "Rājā ca pathaviṁ sabbaṁ,",
  "ja536:95.2_0": "sasamuddaṁ sapabbataṁ;",
  "ja536:95.3_0": "Ajjhāvasaṁ vijinitvā,",
  "ja536:95.4_0": "anantaratanocitaṁ;",
  "ja536:95.5_0": "Pāraṁ samuddaṁ pattheti,",
  "ja536:95.6_0": "ūnattā hi na pūrati.",
  "ja536:96.1_0": "Ekamekāya itthiyā,",
  "ja536:96.2_0": "aṭṭhaṭṭha patino siyā;",
  "ja536:96.3_0": "Sūrā ca balavanto ca,",
  "ja536:96.4_0": "sabbakāmarasāharā;",
  "ja536:96.5_0": "Kareyya navame chandaṁ,",
  "ja536:96.6_0": "ūnattā hi na pūrati.",
  "ja536:97.1_0": "Sabbitthiyo sikhiriva sabbabhakkhā,",
  "ja536:97.2_0": "Sabbitthiyo nadīriva sabbavāhī;",
  "ja536:97.3_0": "Sabbitthiyo kaṇṭakānaṁva sākhā,",
  "ja536:97.4_0": "Sabbitthiyo dhanahetu vajanti.",
  "ja536:98.1_0": "Vātañca jālena naro parāmase,",
  "ja536:98.2_0": "Osiñcaye sāgaramekapāṇinā;",
  "ja536:98.3_0": "Sakena hatthena kareyya ghosaṁ,",
  "ja536:98.4_0": "Yo sabbabhāvaṁ pamadāsu osaje.",
  "ja536:99.1_0": "Corīnaṁ bahubuddhīnaṁ,",
  "ja536:99.2_0": "yāsu saccaṁ sudullabhaṁ;",
  "ja536:99.3_0": "Thīnaṁ bhāvo durājāno,",
  "ja536:99.4_0": "macchassevodake gataṁ.",
  "ja536:100.1_0": "Analā mudusambhāsā,",
  "ja536:100.2_0": "duppūrā tā nadīsamā;",
  "ja536:100.3_0": "Sīdanti naṁ viditvāna,",
  "ja536:100.4_0": "ārakā parivajjaye.",
  "ja536:101.1_0": "Āvaṭṭanī mahāmāyā,",
  "ja536:101.2_0": "brahmacariyavikopanā;",
  "ja536:101.3_0": "Sīdanti naṁ viditvāna,",
  "ja536:101.4_0": "ārakā parivajjaye.",
  "ja536:102.1_0": "Yaṁ etā upasevanti,",
  "ja536:102.2_0": "chandasā vā dhanena vā;",
  "ja536:102.3_0": "Jātavedova saṇṭhānaṁ,",
  "ja536:102.4_0": "khippaṁ anudahanti nan”ti.",
  "ja536:103.1_0": "Atha khalu, bho,",
  "ja536:103.1_1": "kuṇālo sakuṇo nāradassa devabrāhmaṇassa ādimajjhakathāpariyosānaṁ viditvā tāyaṁ velāyaṁ imā gāthāyo abhāsi—",
  "ja536:104.1_0": "“Sallape nisitakhaggapāṇinā,",
  "ja536:104.2_0": "Paṇḍito api pisācadosinā;",
  "ja536:104.3_0": "Uggatejamuragampi āside,",
  "ja536:104.4_0": "Eko ekāya pamadāya nālape.",
  "ja536:105.1_0": "Lokacittamathanā hi nāriyo,",
  "ja536:105.2_0": "Naccagītabhaṇitamhitāvudhā;",
  "ja536:105.3_0": "Bādhayanti anupaṭṭhitassatiṁ,",
  "ja536:105.4_0": "Dīpe rakkhasigaṇova vāṇije.",
  "ja536:106.1_0": "Natthi tāsaṁ vinayo na saṁvaro,",
  "ja536:106.2_0": "Majjamaṁsaniratā asaññatā;",
  "ja536:106.3_0": "Tā gilanti purisassa pābhataṁ,",
  "ja536:106.4_0": "Sāgareva makaraṁ timiṅgalo.",
  "ja536:107.1_0": "Pañcakāmaguṇasātagocarā,",
  "ja536:107.2_0": "Uddhatā aniyatā asaññatā;",
  "ja536:107.3_0": "Osaranti pamadā pamādinaṁ,",
  "ja536:107.4_0": "Loṇatoyavatiyaṁva āpakā.",
  "ja536:108.1_0": "Yaṁ naraṁ upalapenti nāriyo,",
  "ja536:108.2_0": "Chandasā va ratiyā dhanena vā;",
  "ja536:108.3_0": "Jātavedasadisampi tādisaṁ,",
  "ja536:108.4_0": "Rāgadosavadhiyo dahanti naṁ.",
  "ja536:109.1_0": "Aḍḍhaṁ ñatvā purisaṁ mahaddhanaṁ,",
  "ja536:109.2_0": "Osaranti sadhanā sahattanā;",
  "ja536:109.3_0": "Rattacittamativeṭhayanti naṁ,",
  "ja536:109.4_0": "Sāla māluvalatāva kānane.",
  "ja536:110.1_0": "Tā upenti vividhena chandasā,",
  "ja536:110.2_0": "Citrabimbamukhiyo alaṅkatā;",
  "ja536:110.3_0": "Uhasanti pahasanti nāriyo,",
  "ja536:110.4_0": "Sambarova satamāyakovidā.",
  "ja536:111.1_0": "Jātarūpamaṇimuttabhūsitā,",
  "ja536:111.2_0": "Sakkatā patikulesu nāriyo;",
  "ja536:111.3_0": "Rakkhitā aticaranti sāmikaṁ,",
  "ja536:111.4_0": "Dānavaṁva hadayantarassitā.",
  "ja536:112.1_0": "Tejavāpi hi naro vicakkhaṇo,",
  "ja536:112.2_0": "Sakkato bahujanassa pūjito;",
  "ja536:112.3_0": "Nārinaṁ vasagato na bhāsati,",
  "ja536:112.4_0": "Rāhunā upahatova candimā.",
  "ja536:113.1_0": "Yaṁ kareyya kupito diso disaṁ,",
  "ja536:113.2_0": "Duṭṭhacitto vasamāgataṁ ariṁ;",
  "ja536:113.3_0": "Tena bhiyyo byasanaṁ nigacchati,",
  "ja536:113.4_0": "Nārinaṁ vasagato apekkhavā.",
  "ja536:114.1_0": "Kesalūnanakhachinnatajjitā,",
  "ja536:114.2_0": "Pādapāṇikasadaṇḍatāḷitā;",
  "ja536:114.3_0": "Hīnamevupagatā hi nāriyo,",
  "ja536:114.4_0": "Tā ramanti kuṇapeva makkhikā.",
  "ja536:115.1_0": "Tā kulesu visikhantaresu vā,",
  "ja536:115.2_0": "Rājadhāninigamesu vā puna;",
  "ja536:115.3_0": "Oḍḍitaṁ namucipāsavākaraṁ,",
  "ja536:115.4_0": "Cakkhumā parivajje sukhatthiko.",
  "ja536:116.1_0": "Ossajitva kusalaṁ tapoguṇaṁ,",
  "ja536:116.2_0": "Yo anariyacaritāni mācari;",
  "ja536:116.3_0": "Devatāhi nirayaṁ nimissati,",
  "ja536:116.4_0": "Chedagāmimaṇiyaṁva vāṇijo.",
  "ja536:117.1_0": "So idha garahito parattha ca,",
  "ja536:117.2_0": "Dummatī upahato sakammunā;",
  "ja536:117.3_0": "Gacchatī aniyato gaḷāgaḷaṁ,",
  "ja536:117.4_0": "Duṭṭhagadrabharathova uppathe.",
  "ja536:118.1_0": "So upeti nirayaṁ patāpanaṁ,",
  "ja536:118.2_0": "Sattisimbalivanañca āyasaṁ;",
  "ja536:118.3_0": "Āvasitvā tiracchānayoniyaṁ,",
  "ja536:118.4_0": "Petarājavisayaṁ na muñcati.",
  "ja536:119.1_0": "Dibyakhiḍḍaratiyo ca nandane,",
  "ja536:119.2_0": "Cakkavatticaritañca mānuse;",
  "ja536:119.3_0": "Nāsayanti pamadā pamādinaṁ,",
  "ja536:119.4_0": "Duggatiñca paṭipādayanti naṁ.",
  "ja536:120.1_0": "Dibyakhiḍḍaratiyo na dullabhā,",
  "ja536:120.2_0": "Cakkavatticaritañca mānuse;",
  "ja536:120.3_0": "Soṇṇabyamhanilayā ca accharā,",
  "ja536:120.4_0": "Ye caranti pamadāhanatthikā.",
  "ja536:121.1_0": "Kāmadhātusamatikkamā gati,",
  "ja536:121.2_0": "Rūpadhātuyā bhāvo na dullabho;",
  "ja536:121.3_0": "Vītarāgavisayūpapatti yā,",
  "ja536:121.4_0": "Ye caranti pamadāhanatthikā.",
  "ja536:122.1_0": "Sabbadukkhasamatikkamaṁ sivaṁ,",
  "ja536:122.2_0": "Accantamacalitaṁ asaṅkhataṁ;",
  "ja536:122.3_0": "Nibbutehi sucihī na dullabhaṁ,",
  "ja536:122.4_0": "Ye caranti pamadāhanatthikā”ti.",
  "ja536:123.1_0": "“Kuṇālohaṁ tadā āsiṁ,",
  "ja536:123.2_0": "udāyī phussakokilo;",
  "ja536:123.3_0": "Ānando gijjharājāsi,",
  "ja536:123.4_0": "sāriputto ca nārado;",
  "ja536:123.5_0": "Parisā buddhaparisā,",
  "ja536:123.6_0": "evaṁ dhāretha jātakan”ti.",
  "ja536:124.1_0": "Kuṇālajātakaṁ catutthaṁ."
}