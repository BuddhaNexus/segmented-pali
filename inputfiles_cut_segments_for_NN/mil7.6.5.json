{
  "mil7.6.5:0.1_0": "Milindapañha",
  "mil7.6.5:0.2_0": "Opammakathāpañha",
  "mil7.6.5:0.3_0": "Sīhavagga",
  "mil7.6.5:0.4_0": "5. Ulūkaṅgapañha",
  "mil7.6.5:1.1_0": "“Bhante nāgasena,",
  "mil7.6.5:1.1_1": "‘ulūkassa dve aṅgāni gahetabbānī’ti yaṁ vadesi,",
  "mil7.6.5:1.1_2": "katamāni tāni dve aṅgāni gahetabbānī”ti?",
  "mil7.6.5:1.2_0": "“Yathā, mahārāja, ulūko kākehi paṭiviruddho,",
  "mil7.6.5:1.2_1": "rattiṁ kākasaṅghaṁ gantvā bahūpi kāke hanati;",
  "mil7.6.5:1.3_0": "evameva kho, mahārāja,",
  "mil7.6.5:1.3_1": "yoginā yogāvacarena aññāṇena paṭiviruddho kātabbo,",
  "mil7.6.5:1.3_2": "ekena raho nisīditvā aññāṇaṁ sampamadditabbaṁ, mūlato chinditabbaṁ.",
  "mil7.6.5:1.4_0": "Idaṁ, mahārāja,",
  "mil7.6.5:1.4_1": "ulūkassa paṭhamaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.6.5:2.1_0": "Puna caparaṁ, mahārāja, ulūko suppaṭisallīno hoti;",
  "mil7.6.5:2.2_0": "evameva kho, mahārāja,",
  "mil7.6.5:2.2_1": "yoginā yogāvacarena paṭisallānārāmena bhavitabbaṁ paṭisallānaratena.",
  "mil7.6.5:2.3_0": "Idaṁ, mahārāja, ulūkassa dutiyaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.6.5:2.4_0": "Bhāsitampetaṁ, mahārāja,",
  "mil7.6.5:2.4_1": "bhagavatā devātidevena saṁyuttanikāyavare—",
  "mil7.6.5:3.1_0": "‘Idha, bhikkhave,",
  "mil7.6.5:3.1_1": "bhikkhu paṭisallānārāmo paṭisallānarato “idaṁ dukkhan”ti yathābhūtaṁ pajānāti,",
  "mil7.6.5:3.1_2": "“ayaṁ dukkhasamudayo”ti yathābhūtaṁ pajānāti,",
  "mil7.6.5:3.1_3": "“ayaṁ dukkhanirodho”ti yathābhūtaṁ pajānāti,",
  "mil7.6.5:3.1_4": "“ayaṁ dukkhanirodhagāminī paṭipadā”ti yathābhūtaṁ pajānātī’”ti.",
  "mil7.6.5:4.1_0": "Ulūkaṅgapañho pañcamo."
}