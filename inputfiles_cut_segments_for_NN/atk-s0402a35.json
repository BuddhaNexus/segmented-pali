{
  "atk-s0402a35:0_0": "17-18. Peyyālavaggādivaṇṇanā",
  "atk-s0402a35:1_0": "164-184.Samanuññoti samānajjhāsayo.",
  "atk-s0402a35:1_1": "Rāgassāti pañcakāmaguṇikarāgassa.",
  "atk-s0402a35:1_2": "Abhiññāyāti abhijānanatthaṁ.",
  "atk-s0402a35:1_3": "Suññato samādhītiādīhi tīhipi samādhīhi vipassanāva kathitā.",
  "atk-s0402a35:1_4": "Vipassanā hi niccābhinivesa-niccanimitta-niccapaṇidhiādīnaṁ abhāvā imāni nāmāni labhati.",
  "atk-s0402a35:1_5": "Pariññāyāti parijānanatthaṁ.",
  "atk-s0402a35:1_6": "Sesapadesupi eseva nayoti.",
  "atk-s0402a35:2_0": "Peyyālavaggādi niṭṭhitā.",
  "atk-s0402a35:3_0": "Manorathapūraṇiyā aṅguttaranikāya-aṭṭhakathāya",
  "atk-s0402a35:4_0": "Tikanipātassa saṁvaṇṇanā niṭṭhitā.",
  "atk-s0402a35:5_0": " Namo tassa bhagavato arahato sammāsambuddhassa",
  "atk-s0402a35:6_0": "Aṅguttaranikāye",
  "atk-s0402a35:7_0": "Catukkanipāta-aṭṭhakathā",
  "atk-s0402a35:8_0": "1. Paṭhamapaṇṇāsakaṁ"
}