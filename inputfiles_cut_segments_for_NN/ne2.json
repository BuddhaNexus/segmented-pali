{
  "ne2:0.1_0": "Netti",
  "ne2:0.2_0": "2. Uddesavāra",
  "ne2:1.1_0": "Tattha katame soḷasa hārā?",
  "ne2:1.2_0": "Desanā vicayo yutti padaṭṭhāno lakkhaṇo catubyūho āvaṭṭo vibhatti parivattano vevacano paññatti otaraṇo sodhano adhiṭṭhāno parikkhāro samāropano iti.",
  "ne2:2.1_0": "Tassānugīti",
  "ne2:3.1_0": "Desanā vicayo yutti,",
  "ne2:3.2_0": "padaṭṭhāno ca lakkhaṇo;",
  "ne2:3.3_0": "Catubyūho ca āvaṭṭo,",
  "ne2:3.4_0": "vibhatti parivattano.",
  "ne2:4.1_0": "Vevacano ca paññatti,",
  "ne2:4.2_0": "otaraṇo ca sodhano;",
  "ne2:4.3_0": "Adhiṭṭhāno parikkhāro,",
  "ne2:4.4_0": "samāropano soḷaso.",
  "ne2:5.1_0": "Ete soḷasa hārā,",
  "ne2:5.2_0": "Pakittitā atthato asaṅkiṇṇā;",
  "ne2:5.3_0": "Etesañceva bhavati,",
  "ne2:5.4_0": "Vitthāratayā nayavibhattīti.",
  "ne2:6.1_0": "Tattha katame pañca nayā?",
  "ne2:6.2_0": "Nandiyāvaṭṭo tipukkhalo sīhavikkīḷito disālocano aṅkuso iti.",
  "ne2:7.1_0": "Tassānugīti",
  "ne2:8.1_0": "Paṭhamo nandiyāvaṭṭo,",
  "ne2:8.2_0": "dutiyo ca tipukkhalo;",
  "ne2:8.3_0": "Sīhavikkīḷito nāma,",
  "ne2:8.4_0": "tatiyo nayalañjako.",
  "ne2:9.1_0": "Disālocanamāhaṁsu,",
  "ne2:9.2_0": "catutthaṁ nayamuttamaṁ;",
  "ne2:9.3_0": "Pañcamo aṅkuso nāma,",
  "ne2:9.4_0": "sabbe pañca nayā gatāti.",
  "ne2:10.1_0": "Tattha katamāni aṭṭhārasa mūlapadāni?",
  "ne2:10.2_0": "Nava padāni kusalāni nava padāni akusalāni.",
  "ne2:10.3_0": "Tattha katamāni nava padāni akusalāni,",
  "ne2:10.3_1": "taṇhā avijjā lobho doso moho subhasaññā sukhasaññā niccasaññā attasaññāti, imāni nava padāni akusalāni,",
  "ne2:10.3_2": "yattha sabbo akusalapakkho saṅgahaṁ samosaraṇaṁ gacchati.",
  "ne2:11.1_0": "Tattha katamāni nava padāni kusalāni?",
  "ne2:11.2_0": "Samatho vipassanā alobho adoso amoho asubhasaññā dukkhasaññā aniccasaññā anattasaññāti, imāni nava padāni kusalāni,",
  "ne2:11.2_1": "yattha sabbo kusalapakkho saṅgahaṁ samosaraṇaṁ gacchati.",
  "ne2:11.3_0": "Tatridaṁ uddānaṁ",
  "ne2:12.1_0": "Taṇhā ca avijjāpi ca,",
  "ne2:12.2_0": "Lobho doso tatheva moho ca;",
  "ne2:12.3_0": "Caturo ca vipallāsā,",
  "ne2:12.4_0": "Kilesabhūmī nava padāni.",
  "ne2:13.1_0": "Samatho ca vipassanā ca,",
  "ne2:13.2_0": "Kusalāni ca yāni tīṇi mūlāni;",
  "ne2:13.3_0": "Caturo satipaṭṭhānā,",
  "ne2:13.4_0": "Indriyabhūmī nava padāni.",
  "ne2:14.1_0": "Navahi ca padehi kusalā,",
  "ne2:14.2_0": "Navahi ca yujjanti akusalapakkhā;",
  "ne2:14.3_0": "Ete kho mūlapadā,",
  "ne2:14.4_0": "Bhavanti aṭṭhārasa padānīti.",
  "ne2:14.5_0": "Uddesavāro."
}