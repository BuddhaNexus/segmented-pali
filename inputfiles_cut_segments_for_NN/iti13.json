{
  "iti13:0.1_0": "Itivuttaka 13",
  "iti13:0.2_0": "Ekakanipāta",
  "iti13:0.3_0": "Dutiyavagga",
  "iti13:0.4_0": "Makkhapariññāsutta",
  "iti13:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti13:2.1_0": "“Makkhaṁ, bhikkhave,",
  "iti13:2.1_1": "anabhijānaṁ aparijānaṁ tattha cittaṁ avirājayaṁ appajahaṁ abhabbo dukkhakkhayāya.",
  "iti13:2.2_0": "Makkhañca kho, bhikkhave,",
  "iti13:2.2_1": "abhijānaṁ parijānaṁ tattha cittaṁ virājayaṁ pajahaṁ bhabbo dukkhakkhayāyā”ti.",
  "iti13:2.3_0": "Etamatthaṁ bhagavā avoca.",
  "iti13:2.4_0": "Tatthetaṁ iti vuccati:",
  "iti13:3.1_0": "“Yena makkhena makkhāse,",
  "iti13:3.2_0": "sattā gacchanti duggatiṁ;",
  "iti13:3.3_0": "Taṁ makkhaṁ sammadaññāya,",
  "iti13:3.4_0": "pajahanti vipassino;",
  "iti13:3.5_0": "Pahāya na punāyanti,",
  "iti13:3.6_0": "imaṁ lokaṁ kudācanan”ti.",
  "iti13:4.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti13:4.2_0": "Tatiyaṁ."
}