{
  "kv12.5:0.1_0": "Kathāvatthu",
  "kv12.5:0.2_0": "Mahāpaṇṇāsaka",
  "kv12.5:0.3_0": "Dvādasamavagga",
  "kv12.5:0.4_0": "Sattakkhattuparamakathā",
  "kv12.5:1.1_0": "Sattakkhattuparamo puggalo sattakkhattuparamatāniyatoti?",
  "kv12.5:1.2_0": "Āmantā.",
  "kv12.5:1.3_0": "Mātā jīvitā voropitā …",
  "kv12.5:1.4_0": "pitā jīvitā voropito …",
  "kv12.5:1.5_0": "arahā jīvitā voropito …",
  "kv12.5:1.6_0": "duṭṭhena cittena tathāgatassa lohitaṁ uppāditaṁ …",
  "kv12.5:1.7_0": "saṅgho bhinnoti?",
  "kv12.5:1.8_0": "Na hevaṁ vattabbe …pe….",
  "kv12.5:2.1_0": "Sattakkhattuparamo puggalo sattakkhattuparamatāniyatoti?",
  "kv12.5:2.2_0": "Āmantā.",
  "kv12.5:2.3_0": "Abhabbo antarā dhammaṁ abhisametunti?",
  "kv12.5:2.4_0": "Na hevaṁ vattabbe …pe….",
  "kv12.5:2.5_0": "Abhabbo antarā dhammaṁ abhisametunti?",
  "kv12.5:2.6_0": "Āmantā.",
  "kv12.5:2.7_0": "Mātā jīvitā voropitā …",
  "kv12.5:2.8_0": "pitā jīvitā voropito …",
  "kv12.5:2.9_0": "arahā jīvitā voropito …",
  "kv12.5:2.10_0": "duṭṭhena cittena tathāgatassa lohitaṁ uppāditaṁ …",
  "kv12.5:2.11_0": "saṅgho bhinnoti?",
  "kv12.5:2.12_0": "Na hevaṁ vattabbe …pe….",
  "kv12.5:3.1_0": "Sattakkhattuparamo puggalo sattakkhattuparamatāniyatoti?",
  "kv12.5:3.2_0": "Āmantā.",
  "kv12.5:3.3_0": "Atthi so niyamo yena niyamena sattakkhattuparamo puggalo sattakkhattuparamatāniyatoti?",
  "kv12.5:3.4_0": "Na hevaṁ vattabbe …pe…",
  "kv12.5:3.5_0": "atthi te satipaṭṭhānā …pe…",
  "kv12.5:3.6_0": "sammappadhānā …",
  "kv12.5:3.7_0": "iddhipādā …",
  "kv12.5:3.8_0": "indriyā …",
  "kv12.5:3.9_0": "balā …",
  "kv12.5:3.10_0": "bojjhaṅgā yehi bojjhaṅgehi sattakkhattuparamo puggalo sattakkhattuparamatāniyatoti?",
  "kv12.5:3.11_0": "Na hevaṁ vattabbe …pe….",
  "kv12.5:4.1_0": "Natthi so niyamo yena niyamena sattakkhattuparamo puggalo sattakkhattuparamatāniyatoti?",
  "kv12.5:4.2_0": "Āmantā.",
  "kv12.5:4.3_0": "Hañci natthi so niyamo yena niyamena sattakkhattuparamo puggalo sattakkhattuparamatāniyato,",
  "kv12.5:4.3_1": "no ca vata re vattabbe—",
  "kv12.5:4.4_0": "“sattakkhattuparamo puggalo sattakkhattuparamatāniyato”ti.",
  "kv12.5:5.1_0": "Natthi te satipaṭṭhānā …pe…",
  "kv12.5:5.2_0": "bojjhaṅgā yehi bojjhaṅgehi sattakkhattuparamo puggalo sattakkhattuparamatāniyatoti?",
  "kv12.5:5.3_0": "Āmantā.",
  "kv12.5:5.4_0": "Hañci natthi te bojjhaṅgā yehi bojjhaṅgehi sattakkhattuparamo puggalo sattakkhattuparamatāniyato,",
  "kv12.5:5.4_1": "no ca vata re vattabbe—",
  "kv12.5:5.5_0": "“sattakkhattuparamo puggalo sattakkhattuparamatāniyato”ti.",
  "kv12.5:6.1_0": "Sattakkhattuparamo puggalo sattakkhattuparamatāniyatoti?",
  "kv12.5:6.2_0": "Āmantā.",
  "kv12.5:6.3_0": "Sakadāgāminiyamenāti?",
  "kv12.5:6.4_0": "Na hevaṁ vattabbe …pe…",
  "kv12.5:6.5_0": "anāgāminiyamenāti?",
  "kv12.5:6.6_0": "Na hevaṁ vattabbe …pe…",
  "kv12.5:6.7_0": "arahattaniyamenāti?",
  "kv12.5:6.8_0": "Na hevaṁ vattabbe …pe….",
  "kv12.5:7.1_0": "Katamena niyamenāti?",
  "kv12.5:7.2_0": "Sotāpattiniyamenāti.",
  "kv12.5:7.3_0": "Sattakkhattuparamo puggalo sattakkhattuparamatāniyatoti?",
  "kv12.5:7.4_0": "Āmantā.",
  "kv12.5:7.5_0": "Ye keci sotāpattiniyāmaṁ okkamanti, sabbe te sattakkhattuparamatāniyatāti?",
  "kv12.5:7.6_0": "Na hevaṁ vattabbe …pe….",
  "kv12.5:8.1_0": "Na vattabbaṁ—",
  "kv12.5:8.2_0": "“sattakkhattuparamo puggalo sattakkhattuparamatāniyato”ti?",
  "kv12.5:8.3_0": "Āmantā.",
  "kv12.5:8.4_0": "Nanu so sattakkhattuparamoti?",
  "kv12.5:8.5_0": "Āmantā.",
  "kv12.5:8.6_0": "Hañci so sattakkhattuparamo, tena vata re vattabbe—",
  "kv12.5:8.7_0": "“sattakkhattuparamo puggalo sattakkhattuparamatāniyato”ti.",
  "kv12.5:8.8_0": "Sattakkhattuparamakathā niṭṭhitā."
}