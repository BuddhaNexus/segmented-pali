{
  "tha-ap265:0.1_0": "Therāpadāna",
  "tha-ap265:0.2_0": "Padumukkhipavagga",
  "tha-ap265:0.3_0": "3. Aḍḍhacandiyattheraapadāna",
  "tha-ap265:1.1_0": "“Tissassa kho bhagavato,",
  "tha-ap265:1.2_0": "bodhiyā pādaputtame;",
  "tha-ap265:1.3_0": "Aḍḍhacandaṁ mayā dinnaṁ,",
  "tha-ap265:1.4_0": "dharaṇīruhapādape.",
  "tha-ap265:2.1_0": "Dvenavute ito kappe,",
  "tha-ap265:2.2_0": "yaṁ candamabhiropayiṁ;",
  "tha-ap265:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap265:2.4_0": "bodhipūjāyidaṁ phalaṁ.",
  "tha-ap265:3.1_0": "Pañcavīse ito kappe,",
  "tha-ap265:3.2_0": "devalo nāma khattiyo;",
  "tha-ap265:3.3_0": "Sattaratanasampanno,",
  "tha-ap265:3.4_0": "cakkavattī mahabbalo.",
  "tha-ap265:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap265:4.2_0": "…pe…",
  "tha-ap265:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap265:5.1_0": "Itthaṁ sudaṁ āyasmā aḍḍhacandiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap265:6.1_0": "Aḍḍhacandiyattherassāpadānaṁ tatiyaṁ."
}