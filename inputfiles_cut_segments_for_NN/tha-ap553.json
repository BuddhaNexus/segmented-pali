{
  "tha-ap553:0.1_0": "Therāpadāna",
  "tha-ap553:0.2_0": "Yasavagga",
  "tha-ap553:0.3_0": "1. Yasattheraapadāna",
  "tha-ap553:1.1_0": "“Mahāsamuddaṁ oggayha,",
  "tha-ap553:1.2_0": "bhavanaṁ me sunimmitaṁ;",
  "tha-ap553:1.3_0": "Sunimmitā pokkharaṇī,",
  "tha-ap553:1.4_0": "cakkavākūpakūjitā.",
  "tha-ap553:2.1_0": "Mandārakehi sañchannā,",
  "tha-ap553:2.2_0": "padumuppalakehi ca;",
  "tha-ap553:2.3_0": "Nadī ca sandate tattha,",
  "tha-ap553:2.4_0": "supatitthā manoramā.",
  "tha-ap553:3.1_0": "Macchakacchapasañchannā,",
  "tha-ap553:3.2_0": "nānādijasamotthaṭā;",
  "tha-ap553:3.3_0": "Mayūrakoñcābhirudā,",
  "tha-ap553:3.4_0": "kokilādīhi vagguhi.",
  "tha-ap553:4.1_0": "Pārevatā ravihaṁsā,",
  "tha-ap553:4.2_0": "cakkavākā nadīcarā;",
  "tha-ap553:4.3_0": "Tittirā sālikā cettha,",
  "tha-ap553:4.4_0": "pāvakā jīvaṁjīvakā.",
  "tha-ap553:5.1_0": "Haṁsākoñcābhinaditā,",
  "tha-ap553:5.2_0": "kosiyā piṅgalā bahū;",
  "tha-ap553:5.3_0": "Sattaratanasampannā,",
  "tha-ap553:5.4_0": "maṇimuttapavāḷikā.",
  "tha-ap553:6.1_0": "Sabbe soṇṇamayā rukkhā,",
  "tha-ap553:6.2_0": "nānākhandhasameritā;",
  "tha-ap553:6.3_0": "Ujjotenti divārattiṁ,",
  "tha-ap553:6.4_0": "bhavanaṁ sabbakālikaṁ.",
  "tha-ap553:7.1_0": "Saṭṭhituriyasahassāni,",
  "tha-ap553:7.2_0": "sāyaṁ pāto pavajjare;",
  "tha-ap553:7.3_0": "Soḷasitthisahassāni,",
  "tha-ap553:7.4_0": "parivārenti maṁ sadā.",
  "tha-ap553:8.1_0": "Abhinikkhamma bhavanā,",
  "tha-ap553:8.2_0": "sumedhaṁ lokanāyakaṁ;",
  "tha-ap553:8.3_0": "Pasannacitto sumano,",
  "tha-ap553:8.4_0": "vandayiṁ taṁ mahāyasaṁ.",
  "tha-ap553:9.1_0": "Sambuddhaṁ abhivādetvā,",
  "tha-ap553:9.2_0": "sasaṅghaṁ taṁ nimantayiṁ;",
  "tha-ap553:9.3_0": "Adhivāsesi so dhīro,",
  "tha-ap553:9.4_0": "sumedho lokanāyako.",
  "tha-ap553:10.1_0": "Mama dhammakathaṁ katvā,",
  "tha-ap553:10.2_0": "uyyojesi mahāmuni;",
  "tha-ap553:10.3_0": "Sambuddhaṁ abhivādetvā,",
  "tha-ap553:10.4_0": "bhavanaṁ me upāgamiṁ.",
  "tha-ap553:11.1_0": "Āmantayiṁ parijanaṁ,",
  "tha-ap553:11.2_0": "sabbe sannipatuṁ tadā;",
  "tha-ap553:11.3_0": "‘Pubbaṇhasamayaṁ buddho,",
  "tha-ap553:11.4_0": "bhavanaṁ āgamissati.",
  "tha-ap553:12.1_0": "Lābhā amhaṁ suladdhā no,",
  "tha-ap553:12.2_0": "ye vasāma tavantike;",
  "tha-ap553:12.3_0": "Mayampi buddhaseṭṭhassa,",
  "tha-ap553:12.4_0": "pūjayissāma satthuno’.",
  "tha-ap553:13.1_0": "Annaṁ pānaṁ paṭṭhapetvā,",
  "tha-ap553:13.2_0": "kālaṁ ārocayiṁ ahaṁ;",
  "tha-ap553:13.3_0": "Vasīsatasahassehi,",
  "tha-ap553:13.4_0": "upesi lokanāyako.",
  "tha-ap553:14.1_0": "Pañcaṅgikehi turiyehi,",
  "tha-ap553:14.2_0": "paccuggamamakāsahaṁ;",
  "tha-ap553:14.3_0": "Sabbasoṇṇamaye pīṭhe,",
  "tha-ap553:14.4_0": "nisīdi purisuttamo.",
  "tha-ap553:15.1_0": "Uparicchadanaṁ āsi,",
  "tha-ap553:15.2_0": "sabbasoṇṇamayaṁ tadā;",
  "tha-ap553:15.3_0": "Bījanīyo pavāyanti,",
  "tha-ap553:15.4_0": "bhikkhusaṅghaṁ anuttaraṁ.",
  "tha-ap553:16.1_0": "Pahūtenannapānena,",
  "tha-ap553:16.2_0": "bhikkhusaṅghaṁ atappayiṁ;",
  "tha-ap553:16.3_0": "Paccekadussayugale,",
  "tha-ap553:16.4_0": "bhikkhusaṅghassadāsahaṁ.",
  "tha-ap553:17.1_0": "Yaṁ vadeti sumedho so,",
  "tha-ap553:17.2_0": "āhutīnaṁ paṭiggaho;",
  "tha-ap553:17.3_0": "Bhikkhusaṅghe nisīditvā,",
  "tha-ap553:17.4_0": "imā gāthā abhāsatha.",
  "tha-ap553:18.1_0": "‘Yo maṁ annena pānena,",
  "tha-ap553:18.2_0": "sabbe ime ca tappayi;",
  "tha-ap553:18.3_0": "Tamahaṁ kittayissāmi,",
  "tha-ap553:18.4_0": "suṇātha mama bhāsato.",
  "tha-ap553:19.1_0": "Aṭṭhārase kappasate,",
  "tha-ap553:19.2_0": "devaloke ramissati;",
  "tha-ap553:19.3_0": "Sahassakkhattuṁ rājāyaṁ,",
  "tha-ap553:19.4_0": "cakkavattī bhavissati.",
  "tha-ap553:20.1_0": "Upagacchati yaṁ yoniṁ,",
  "tha-ap553:20.2_0": "devattaṁ atha mānusaṁ;",
  "tha-ap553:20.3_0": "Sabbasoṇṇamayaṁ tassa,",
  "tha-ap553:20.4_0": "chadanaṁ dhārayissati.",
  "tha-ap553:21.1_0": "Tiṁsakappasahassamhi,",
  "tha-ap553:21.2_0": "okkākakulasambhavo;",
  "tha-ap553:21.3_0": "Gotamo nāma gottena,",
  "tha-ap553:21.4_0": "satthā loke bhavissati.",
  "tha-ap553:22.1_0": "Tassa dhammesu dāyādo,",
  "tha-ap553:22.2_0": "oraso dhammanimmito;",
  "tha-ap553:22.3_0": "Sabbāsave pariññāya,",
  "tha-ap553:22.4_0": "nibbāyissatināsavo.",
  "tha-ap553:23.1_0": "Bhikkhusaṅghe nisīditvā,",
  "tha-ap553:23.2_0": "sīhanādaṁ nadissati;",
  "tha-ap553:23.3_0": "Citake chattaṁ dhārenti,",
  "tha-ap553:23.4_0": "heṭṭhā chattamhi ḍayhatha’.",
  "tha-ap553:24.1_0": "Sāmaññaṁ me anuppattaṁ,",
  "tha-ap553:24.2_0": "kilesā jhāpitā mayā;",
  "tha-ap553:24.3_0": "Maṇḍape rukkhamūle vā,",
  "tha-ap553:24.4_0": "santāso me na vijjati.",
  "tha-ap553:25.1_0": "Tiṁsakappasahassamhi,",
  "tha-ap553:25.2_0": "yaṁ dānamadadiṁ tadā;",
  "tha-ap553:25.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap553:25.4_0": "sabbadānassidaṁ phalaṁ.",
  "tha-ap553:26.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap553:26.2_0": "bhavā sabbe samūhatā;",
  "tha-ap553:26.3_0": "Nāgova bandhanaṁ chetvā,",
  "tha-ap553:26.4_0": "viharāmi anāsavo.",
  "tha-ap553:27.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap553:27.2_0": "mama buddhassa santike;",
  "tha-ap553:27.3_0": "Tisso vijjā anuppattā,",
  "tha-ap553:27.4_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap553:28.1_0": "Paṭisambhidā catasso,",
  "tha-ap553:28.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap553:28.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap553:28.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap553:29.1_0": "Itthaṁ sudaṁ āyasmā yaso thero imā gāthāyo abhāsitthāti.",
  "tha-ap553:30.1_0": "Yasattherassāpadānaṁ paṭhamaṁ."
}