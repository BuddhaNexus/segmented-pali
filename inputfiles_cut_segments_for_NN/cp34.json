{
  "cp34:0.1_0": "Cariyāpiṭaka",
  "cp34:0.2_0": "Yudhañjayavagga",
  "cp34:0.3_0": "Mettāpāramī 2",
  "cp34:0.4_0": "Ekarājacariya",
  "cp34:1.1_0": "“Punāparaṁ yadā homi,",
  "cp34:1.2_0": "ekarājāti vissuto;",
  "cp34:1.3_0": "Paramaṁ sīlaṁ adhiṭṭhāya,",
  "cp34:1.4_0": "pasāsāmi mahāmahiṁ.",
  "cp34:2.1_0": "Dasa kusalakammapathe,",
  "cp34:2.2_0": "vattāmi anavasesato;",
  "cp34:2.3_0": "Catūhi saṅgahavatthūhi,",
  "cp34:2.4_0": "saṅgaṇhāmi mahājanaṁ.",
  "cp34:3.1_0": "Evaṁ me appamattassa,",
  "cp34:3.2_0": "idha loke parattha ca;",
  "cp34:3.3_0": "Dabbaseno upagantvā,",
  "cp34:3.4_0": "acchindanto puraṁ mama.",
  "cp34:4.1_0": "Rājūpajīve nigame,",
  "cp34:4.2_0": "sabalaṭṭhe saraṭṭhake;",
  "cp34:4.3_0": "Sabbaṁ hatthagataṁ katvā,",
  "cp34:4.4_0": "kāsuyā nikhaṇī mamaṁ.",
  "cp34:5.1_0": "Amaccamaṇḍalaṁ rajjaṁ,",
  "cp34:5.2_0": "phītaṁ antepuraṁ mama;",
  "cp34:5.3_0": "Acchinditvāna gahitaṁ,",
  "cp34:5.4_0": "piyaṁ puttaṁva passahaṁ;",
  "cp34:5.5_0": "Mettāya me samo natthi,",
  "cp34:5.6_0": "esā me mettāpāramī”ti.",
  "cp34:6.1_0": "Ekarājacariyaṁ cuddasamaṁ."
}