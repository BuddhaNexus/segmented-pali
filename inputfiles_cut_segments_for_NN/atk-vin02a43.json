{
  "atk-vin02a43:0_0": "Khandhakapucchāvāro",
  "atk-vin02a43:1_0": "Pucchāvissajjanāvaṇṇanā",
  "atk-vin02a43:2_0": "320.Upasampadaṁpucchissanti upasampadakkhandhakaṁ pucchissaṁ.",
  "atk-vin02a43:2_1": "Sanidānaṁ saniddesanti nidānena ca niddesena ca saddhiṁ pucchissāmi.",
  "atk-vin02a43:2_2": "Samukkaṭṭhapadānaṁ kati āpattiyoti yāni tattha samukkaṭṭhāni uttamāni padāni vuttāni,",
  "atk-vin02a43:2_3": "tesaṁ samukkaṭṭhapadānaṁ uttamapadānaṁ saṅkhepato kati āpattiyo hontīti.",
  "atk-vin02a43:2_4": "Yena yena hi padena yā yā āpatti paññattā,",
  "atk-vin02a43:2_5": "sā sā tassa tassa padassa āpattīti vuccati.",
  "atk-vin02a43:2_6": "Tena vuttaṁ ‘‘samukkaṭṭhapadānaṁ kati āpattiyo’’ti.",
  "atk-vin02a43:2_7": "Dve āpattiyoti ūnavīsativassaṁ upasampādentassa pācittiyaṁ,",
  "atk-vin02a43:2_8": "sesesu sabbapadesu dukkaṭaṁ.",
  "atk-vin02a43:3_0": "Tissoti ‘‘nassantete vinassantete,",
  "atk-vin02a43:3_1": "ko tehi attho’’ti bhedapurekkhārānaṁ uposathakaraṇe thullaccayaṁ,",
  "atk-vin02a43:3_2": "ukkhittakena saddhiṁ uposathakaraṇe pācittiyaṁ,",
  "atk-vin02a43:3_3": "sesesu dukkaṭanti evaṁ uposathakkhandhake tisso āpattiyo.",
  "atk-vin02a43:3_4": "Ekāti vassūpanāyikakkhandhake ekā dukkaṭāpattiyeva.",
  "atk-vin02a43:4_0": "Tissoti bhedapurekkhārassa pavārayato thullaccayaṁ,",
  "atk-vin02a43:4_1": "ukkhittakena saddhiṁ pācittiyaṁ,",
  "atk-vin02a43:4_2": "sesesu dukkaṭanti evaṁ pavāraṇākkhandhakepi tisso āpattiyo.",
  "atk-vin02a43:5_0": "Tissoti vacchatariṁ uggahetvā mārentānaṁ pācittiyaṁ,",
  "atk-vin02a43:5_1": "rattena cittena aṅgajātachupane thullaccayaṁ,",
  "atk-vin02a43:5_2": "sesesu dukkaṭanti evaṁ cammasaṁyuttepi tisso āpattiyo.",
  "atk-vin02a43:5_3": "Bhesajjakkhandhakepi samantā dvaṅgule thullaccayaṁ,",
  "atk-vin02a43:5_4": "bhojjayāguyā pācittiyaṁ,",
  "atk-vin02a43:5_5": "sesesu dukkaṭanti evaṁ tisso āpattiyo.",
  "atk-vin02a43:6_0": "Kathinaṁ kevalaṁ paññattimeva,",
  "atk-vin02a43:6_1": "natthi tattha āpatti.",
  "atk-vin02a43:6_2": "Cīvarasaṁyutte kusacīravākacīresu thullaccayaṁ,",
  "atk-vin02a43:6_3": "atirekacīvare nissaggiyaṁ,",
  "atk-vin02a43:6_4": "sesesu dukkaṭanti imā tisso āpattiyo.",
  "atk-vin02a43:7_0": "Campeyyake ekā dukkaṭāpattiyeva.",
  "atk-vin02a43:7_1": "Kosambaka-kammakkhandhaka-pārivāsikasamuccayakkhandhakesupi ekā dukkaṭāpattiyeva.",
  "atk-vin02a43:8_0": "Samathakkhandhake chandadāyako khiyyati,",
  "atk-vin02a43:8_1": "khiyyanakaṁ pācittiyaṁ,",
  "atk-vin02a43:8_2": "sesesu dukkaṭanti imā dve āpattiyo.",
  "atk-vin02a43:8_3": "Khuddakavatthuke attano aṅgajātaṁ chindati,",
  "atk-vin02a43:8_4": "thullaccayaṁ,",
  "atk-vin02a43:8_5": "romaṭṭhe pācittiyaṁ,",
  "atk-vin02a43:8_6": "sesesu dukkaṭanti imā tisso āpattiyo.",
  "atk-vin02a43:8_7": "Senāsanakkhandhake garubhaṇḍavissajjane thullaccayaṁ,",
  "atk-vin02a43:8_8": "saṅghikā vihārā nikkaḍḍhane pācittiyaṁ,",
  "atk-vin02a43:8_9": "sesesu dukkaṭanti imā tisso āpattiyo.",
  "atk-vin02a43:9_0": "Saṅghabhedebhedakānuvattakānaṁ thullaccayaṁ,",
  "atk-vin02a43:9_1": "gaṇabhojane pācittiyanti imā dve āpattiyo.",
  "atk-vin02a43:9_2": "Samācāraṁ pucchissanti vutte vattakkhandhake ekā dukkaṭāpattiyeva.",
  "atk-vin02a43:9_3": "Sā sabbavattesu anādariyena hoti.",
  "atk-vin02a43:9_4": "Tathā pātimokkhaṭṭhapane.",
  "atk-vin02a43:9_5": "Bhikkhunikkhandhake appavāraṇāya pācittiyaṁ,",
  "atk-vin02a43:9_6": "sesesu dukkaṭanti dve āpattiyo.",
  "atk-vin02a43:9_7": "Pañcasatikasattasatikesu kevalaṁ dhammo saṅgahaṁ āropito,",
  "atk-vin02a43:9_8": "natthi tattha āpattīti.",
  "atk-vin02a43:10_0": "Khandhakapucchāvāravaṇṇanā niṭṭhitā."
}