{
  "ja445:0.1_0": "Jātaka",
  "ja445:0.2_0": "Dasakanipāta",
  "ja445:0.3_0": "Catudvāravagga",
  "ja445:0.4_0": "7. Nigrodhajātaka",
  "ja445:1.1_0": "“Na vāhametaṁ jānāmi,",
  "ja445:1.2_0": "ko vāyaṁ kassa vāti vā;",
  "ja445:1.3_0": "Yathā sākho cari evaṁ,",
  "ja445:1.4_0": "nigrodha kinti maññasi.",
  "ja445:2.1_0": "Tato galavinītena,",
  "ja445:2.2_0": "purisā nīhariṁsu maṁ;",
  "ja445:2.3_0": "Datvā mukhapahārāni,",
  "ja445:2.4_0": "sākhassa vacanaṅkarā.",
  "ja445:3.1_0": "Etādisaṁ dummatinā,",
  "ja445:3.2_0": "akataññuna dubbhinā;",
  "ja445:3.3_0": "Kataṁ anariyaṁ sākhena,",
  "ja445:3.4_0": "sakhinā te janādhipa”.",
  "ja445:4.1_0": "“Na vāhametaṁ jānāmi,",
  "ja445:4.2_0": "napi me koci saṁsati;",
  "ja445:4.3_0": "Yaṁ me tvaṁ samma akkhāsi,",
  "ja445:4.4_0": "sākhena kāraṇaṁ kataṁ.",
  "ja445:5.1_0": "Sakhīnaṁ sājīvakaro,",
  "ja445:5.2_0": "mama sākhassa cūbhayaṁ;",
  "ja445:5.3_0": "Tvaṁ nosissariyaṁ dātā,",
  "ja445:5.4_0": "manussesu mahantataṁ;",
  "ja445:5.5_0": "Tayāmā labbhitā iddhī,",
  "ja445:5.6_0": "ettha me natthi saṁsayo.",
  "ja445:6.1_0": "Yathāpi bījamaggimhi,",
  "ja445:6.2_0": "ḍayhati na virūhati;",
  "ja445:6.3_0": "Evaṁ kataṁ asappurise,",
  "ja445:6.4_0": "nassati na virūhati.",
  "ja445:7.1_0": "Kataññumhi ca posamhi,",
  "ja445:7.2_0": "Sīlavante ariyavuttine;",
  "ja445:7.3_0": "Sukhette viya bījāni,",
  "ja445:7.4_0": "Kataṁ tamhi na nassati”.",
  "ja445:8.1_0": "“Imaṁ jammaṁ nekatikaṁ,",
  "ja445:8.2_0": "asappurisacintakaṁ;",
  "ja445:8.3_0": "Hanantu sākhaṁ sattīhi,",
  "ja445:8.4_0": "nāssa icchāmi jīvitaṁ”.",
  "ja445:9.1_0": "“Khamatassa mahārāja,",
  "ja445:9.2_0": "Pāṇā na paṭiānayā;",
  "ja445:9.3_0": "Khama deva asappurisassa,",
  "ja445:9.4_0": "Nāssa icchāmahaṁ vadhaṁ”.",
  "ja445:10.1_0": "“Nigrodhameva seveyya,",
  "ja445:10.2_0": "na sākhamupasaṁvase;",
  "ja445:10.3_0": "Nigrodhasmiṁ mataṁ seyyo,",
  "ja445:10.4_0": "yañce sākhasmi jīvitan”ti.",
  "ja445:11.1_0": "Nigrodhajātakaṁ sattamaṁ."
}