{
  "tha-ap500:0.1_0": "Therāpadāna",
  "tha-ap500:0.2_0": "Kiṅkaṇipupphavagga",
  "tha-ap500:0.3_0": "8 Pulinathūpiyattheraapadāna",
  "tha-ap500:1.1_0": "“Himavantassāvidūre,",
  "tha-ap500:1.2_0": "yamako nāma pabbato;",
  "tha-ap500:1.3_0": "Assamo sukato mayhaṁ,",
  "tha-ap500:1.4_0": "paṇṇasālā sumāpitā.",
  "tha-ap500:2.1_0": "Nārado nāma nāmena,",
  "tha-ap500:2.2_0": "jaṭilo uggatāpano;",
  "tha-ap500:2.3_0": "Catuddasasahassāni,",
  "tha-ap500:2.4_0": "sissā paricaranti maṁ.",
  "tha-ap500:3.1_0": "Paṭisallīnako santo,",
  "tha-ap500:3.2_0": "evaṁ cintesahaṁ tadā;",
  "tha-ap500:3.3_0": "‘Sabbo jano maṁ pūjeti,",
  "tha-ap500:3.4_0": "nāhaṁ pūjemi kiñcanaṁ.",
  "tha-ap500:4.1_0": "Na me ovādako atthi,",
  "tha-ap500:4.2_0": "vattā koci na vijjati;",
  "tha-ap500:4.3_0": "Anācariyupajjhāyo,",
  "tha-ap500:4.4_0": "vane vāsaṁ upemahaṁ.",
  "tha-ap500:5.1_0": "Upāsamāno yamahaṁ,",
  "tha-ap500:5.2_0": "garucittaṁ upaṭṭhahe;",
  "tha-ap500:5.3_0": "So me ācariyo natthi,",
  "tha-ap500:5.4_0": "vanavāso niratthako.",
  "tha-ap500:6.1_0": "Āyāgaṁ me gavesissaṁ,",
  "tha-ap500:6.2_0": "garuṁ bhāvaniyaṁ tathā;",
  "tha-ap500:6.3_0": "Sāvassayo vasissāmi,",
  "tha-ap500:6.4_0": "na koci garahissati’.",
  "tha-ap500:7.1_0": "Uttānakūlā nadikā,",
  "tha-ap500:7.2_0": "supatitthā manoramā;",
  "tha-ap500:7.3_0": "Saṁsuddhapulinākiṇṇā,",
  "tha-ap500:7.4_0": "avidūre mamassamaṁ.",
  "tha-ap500:8.1_0": "Nadiṁ amarikaṁ nāma,",
  "tha-ap500:8.2_0": "upagantvānahaṁ tadā;",
  "tha-ap500:8.3_0": "Saṁvaḍḍhayitvā pulinaṁ,",
  "tha-ap500:8.4_0": "akaṁ pulinacetiyaṁ.",
  "tha-ap500:9.1_0": "Ye te ahesuṁ sambuddhā,",
  "tha-ap500:9.2_0": "bhavantakaraṇā munī;",
  "tha-ap500:9.3_0": "Tesaṁ etādiso thūpo,",
  "tha-ap500:9.4_0": "taṁ nimittaṁ karomahaṁ.",
  "tha-ap500:10.1_0": "Karitvā pulinaṁ thūpaṁ,",
  "tha-ap500:10.2_0": "sovaṇṇaṁ māpayiṁ ahaṁ;",
  "tha-ap500:10.3_0": "Soṇṇakiṅkaṇipupphāni,",
  "tha-ap500:10.4_0": "sahasse tīṇi pūjayiṁ.",
  "tha-ap500:11.1_0": "Sāyaṁ pātaṁ namassāmi,",
  "tha-ap500:11.2_0": "vedajāto katañjalī;",
  "tha-ap500:11.3_0": "Sammukhā viya sambuddhaṁ,",
  "tha-ap500:11.4_0": "vandiṁ pulinacetiyaṁ.",
  "tha-ap500:12.1_0": "Yadā kilesā jāyanti,",
  "tha-ap500:12.2_0": "vitakkā gehanissitā;",
  "tha-ap500:12.3_0": "Sarāmi sukataṁ thūpaṁ,",
  "tha-ap500:12.4_0": "paccavekkhāmi tāvade.",
  "tha-ap500:13.1_0": "Upanissāya viharaṁ,",
  "tha-ap500:13.2_0": "satthavāhaṁ vināyakaṁ;",
  "tha-ap500:13.3_0": "Kilese saṁvaseyyāsi,",
  "tha-ap500:13.4_0": "na yuttaṁ tava mārisa.",
  "tha-ap500:14.1_0": "Saha āvajjite thūpe,",
  "tha-ap500:14.2_0": "gāravaṁ hoti me tadā;",
  "tha-ap500:14.3_0": "Kuvitakke vinodesiṁ,",
  "tha-ap500:14.4_0": "nāgo tuttaṭṭito yathā.",
  "tha-ap500:15.1_0": "Evaṁ viharamānaṁ maṁ,",
  "tha-ap500:15.2_0": "maccurājābhimaddatha;",
  "tha-ap500:15.3_0": "Tattha kālaṅkato santo,",
  "tha-ap500:15.4_0": "brahmalokamagacchahaṁ.",
  "tha-ap500:16.1_0": "Yāvatāyuṁ vasitvāna,",
  "tha-ap500:16.2_0": "tidive upapajjahaṁ;",
  "tha-ap500:16.3_0": "Asītikkhattuṁ devindo,",
  "tha-ap500:16.4_0": "devarajjamakārayiṁ.",
  "tha-ap500:17.1_0": "Satānaṁ tīṇikkhattuñca,",
  "tha-ap500:17.2_0": "cakkavattī ahosahaṁ;",
  "tha-ap500:17.3_0": "Padesarajjaṁ vipulaṁ,",
  "tha-ap500:17.4_0": "gaṇanāto asaṅkhiyaṁ.",
  "tha-ap500:18.1_0": "Soṇṇakiṅkaṇipupphānaṁ,",
  "tha-ap500:18.2_0": "vipākaṁ anubhomahaṁ;",
  "tha-ap500:18.3_0": "Dhātīsatasahassāni,",
  "tha-ap500:18.4_0": "parivārenti maṁ bhave.",
  "tha-ap500:19.1_0": "Thūpassa pariciṇṇattā,",
  "tha-ap500:19.2_0": "rajojallaṁ na limpati;",
  "tha-ap500:19.3_0": "Gatte sedā na muccanti,",
  "tha-ap500:19.4_0": "suppabhāso bhavāmahaṁ.",
  "tha-ap500:20.1_0": "Aho me sukato thūpo,",
  "tha-ap500:20.2_0": "sudiṭṭhā marikā nadī;",
  "tha-ap500:20.3_0": "Thūpaṁ katvāna pulinaṁ,",
  "tha-ap500:20.4_0": "pattomhi acalaṁ padaṁ.",
  "tha-ap500:21.1_0": "Kusalaṁ kattukāmena,",
  "tha-ap500:21.2_0": "jantunā sāragāhinā;",
  "tha-ap500:21.3_0": "Natthi khettaṁ akhettaṁ vā,",
  "tha-ap500:21.4_0": "paṭipattīva sādhakā.",
  "tha-ap500:22.1_0": "Yathāpi balavā poso,",
  "tha-ap500:22.2_0": "aṇṇavaṁ taritussahe;",
  "tha-ap500:22.3_0": "Parittaṁ kaṭṭhamādāya,",
  "tha-ap500:22.4_0": "pakkhandeyya mahāsaraṁ.",
  "tha-ap500:23.1_0": "Imāhaṁ kaṭṭhaṁ nissāya,",
  "tha-ap500:23.2_0": "tarissāmi mahodadhiṁ;",
  "tha-ap500:23.3_0": "Ussāhena vīriyena,",
  "tha-ap500:23.4_0": "tareyya udadhiṁ naro.",
  "tha-ap500:24.1_0": "Tatheva me kataṁ kammaṁ,",
  "tha-ap500:24.2_0": "parittaṁ thokakañca yaṁ;",
  "tha-ap500:24.3_0": "Taṁ kammaṁ upanissāya,",
  "tha-ap500:24.4_0": "saṁsāraṁ samatikkamiṁ.",
  "tha-ap500:25.1_0": "Pacchime bhave sampatte,",
  "tha-ap500:25.2_0": "sukkamūlena codito;",
  "tha-ap500:25.3_0": "Sāvatthiyaṁ pure jāto,",
  "tha-ap500:25.4_0": "mahāsāle suaḍḍhake.",
  "tha-ap500:26.1_0": "Saddhā mātā pitā mayhaṁ,",
  "tha-ap500:26.2_0": "buddhassa saraṇaṁ gatā;",
  "tha-ap500:26.3_0": "Ubho diṭṭhapadā ete,",
  "tha-ap500:26.4_0": "anuvattanti sāsanaṁ.",
  "tha-ap500:27.1_0": "Bodhipapaṭikaṁ gayha,",
  "tha-ap500:27.2_0": "soṇṇathūpamakārayuṁ;",
  "tha-ap500:27.3_0": "Sāyaṁ pātaṁ namassanti,",
  "tha-ap500:27.4_0": "sakyaputtassa sammukhā.",
  "tha-ap500:28.1_0": "Uposathamhi divase,",
  "tha-ap500:28.2_0": "soṇṇathūpaṁ vinīharuṁ;",
  "tha-ap500:28.3_0": "Buddhassa vaṇṇaṁ kittentā,",
  "tha-ap500:28.4_0": "tiyāmaṁ vītināmayuṁ.",
  "tha-ap500:29.1_0": "Saha disvānahaṁ thūpaṁ,",
  "tha-ap500:29.2_0": "sariṁ pulinacetiyaṁ;",
  "tha-ap500:29.3_0": "Ekāsane nisīditvā,",
  "tha-ap500:29.4_0": "arahattamapāpuṇiṁ.",
  "tha-ap500:30.1_0": "Dvāvīsatimaṁ bhāṇavāraṁ.",
  "tha-ap500:31.1_0": "Gavesamāno taṁ vīraṁ,",
  "tha-ap500:31.2_0": "dhammasenāpatiddasaṁ;",
  "tha-ap500:31.3_0": "Agārā nikkhamitvāna,",
  "tha-ap500:31.4_0": "pabbajiṁ tassa santike.",
  "tha-ap500:32.1_0": "Jātiyā sattavassena,",
  "tha-ap500:32.2_0": "arahattamapāpuṇiṁ;",
  "tha-ap500:32.3_0": "Upasampādayī buddho,",
  "tha-ap500:32.4_0": "guṇamaññāya cakkhumā.",
  "tha-ap500:33.1_0": "Dārakeneva santena,",
  "tha-ap500:33.2_0": "kiriyaṁ niṭṭhitaṁ mayā;",
  "tha-ap500:33.3_0": "Kataṁ me karaṇīyajja,",
  "tha-ap500:33.4_0": "sakyaputtassa sāsane.",
  "tha-ap500:34.1_0": "Sabbaverabhayātīto,",
  "tha-ap500:34.2_0": "sabbasaṅgātigo isi;",
  "tha-ap500:34.3_0": "Sāvako te mahāvīra,",
  "tha-ap500:34.4_0": "soṇṇathūpassidaṁ phalaṁ.",
  "tha-ap500:35.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap500:35.2_0": "…pe…",
  "tha-ap500:35.3_0": "viharāmi anāsavo.",
  "tha-ap500:36.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap500:36.2_0": "…pe…",
  "tha-ap500:36.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap500:37.1_0": "Paṭisambhidā catasso,",
  "tha-ap500:37.2_0": "…pe…",
  "tha-ap500:37.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap500:38.1_0": "Itthaṁ sudaṁ āyasmā pulinathūpiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap500:39.1_0": "Pulinathūpiyattherassāpadānaṁ aṭṭhamaṁ."
}