{
  "patthana1.19:0.1_0": "Paṭṭhānapakaraṇa",
  "patthana1.19:0.2_0": "Dhammānuloma (1), Tikapaṭṭhānapāḷi (1)",
  "patthana1.19:0.3_0": "1.19. Atītattika",
  "patthana1.19:1.0.1_0": "Vibhaṅgavāra",
  "patthana1.19:1.0.2_0": "Hetu",
  "patthana1.19:1.1_0": "Paccuppanno dhammo paccuppannassa dhammassa hetupaccayena paccayo—paccuppannā hetū sampayuttakānaṁ khandhānaṁ",
  "patthana1.19:1.1_1": "cittasamuṭṭhānānañca rūpānaṁ hetupaccayena paccayo.",
  "patthana1.19:1.1_2": "Paṭisandhikkhaṇe …pe….",
  "patthana1.19:2.0_0": "Ārammaṇa",
  "patthana1.19:2.1_0": "Atīto dhammo paccuppannassa dhammassa ārammaṇapaccayena paccayo—dānaṁ datvā sīlaṁ samādiyitvā uposathakammaṁ …pe…",
  "patthana1.19:2.1_1": "paccavekkhati,",
  "patthana1.19:2.1_2": "pubbe suciṇṇāni paccavekkhati,",
  "patthana1.19:2.1_3": "jhānā vuṭṭhahitvā jhānaṁ paccavekkhati.",
  "patthana1.19:2.1_4": "Ariyā maggā vuṭṭhahitvā maggaṁ paccavekkhanti,",
  "patthana1.19:2.1_5": "phalaṁ paccavekkhanti,",
  "patthana1.19:2.1_6": "pahīne kilese paccavekkhanti,",
  "patthana1.19:2.1_7": "vikkhambhite kilese paccavekkhanti,",
  "patthana1.19:2.1_8": "pubbe samudāciṇṇe kilese jānanti.",
  "patthana1.19:2.1_9": "Atītaṁ cakkhuṁ aniccato dukkhato anattato vipassati …pe…",
  "patthana1.19:2.1_10": "domanassaṁ uppajjati.",
  "patthana1.19:2.1_11": "Atītaṁ sotaṁ …pe… ghānaṁ … jivhaṁ … kāyaṁ … rūpe … sadde … gandhe … rase …",
  "patthana1.19:2.1_12": "phoṭṭhabbe … vatthuṁ …",
  "patthana1.19:2.1_13": "atīte khandhe aniccato dukkhato anattato vipassati,",
  "patthana1.19:2.1_14": "assādeti abhinandati,",
  "patthana1.19:2.1_15": "taṁ ārabbha rāgo uppajjati,",
  "patthana1.19:2.1_16": "diṭṭhi uppajjati,",
  "patthana1.19:2.1_17": "vicikicchā …",
  "patthana1.19:2.1_18": "uddhaccaṁ …",
  "patthana1.19:2.1_19": "domanassaṁ uppajjati,",
  "patthana1.19:2.1_20": "ākāsānañcāyatanaṁ viññāṇañcāyatanassa ārammaṇapaccayena paccayo.",
  "patthana1.19:2.1_21": "Ākiñcaññāyatanaṁ nevasaññānāsaññāyatanassa ārammaṇapaccayena paccayo.",
  "patthana1.19:2.1_22": "Atītā khandhā iddhividhañāṇassa,",
  "patthana1.19:2.1_23": "cetopariyañāṇassa,",
  "patthana1.19:2.1_24": "pubbenivāsānussatiñāṇassa,",
  "patthana1.19:2.1_25": "yathākammūpagañāṇassa,",
  "patthana1.19:2.1_26": "āvajjanāya ārammaṇapaccayena paccayo.",
  "patthana1.19:3.1_0": "Anāgato dhammo paccuppannassa dhammassa ārammaṇapaccayena paccayo—anāgataṁ cakkhuṁ …pe… vatthuṁ …",
  "patthana1.19:3.1_1": "anāgate khandhe aniccato …pe…",
  "patthana1.19:3.1_2": "domanassaṁ uppajjati.",
  "patthana1.19:3.1_3": "Anāgatā khandhā iddhividhañāṇassa,",
  "patthana1.19:3.1_4": "cetopariyañāṇassa,",
  "patthana1.19:3.1_5": "anāgataṁsañāṇassa,",
  "patthana1.19:3.1_6": "āvajjanāya ārammaṇapaccayena paccayo.",
  "patthana1.19:4.1_0": "Paccuppanno dhammo paccuppannassa dhammassa ārammaṇapaccayena paccayo— paccuppannaṁ cakkhuṁ …pe… kāyaṁ … rūpe … sadde … gandhe … rase …",
  "patthana1.19:4.1_1": "phoṭṭhabbe … vatthuṁ …",
  "patthana1.19:4.1_2": "paccuppanne khandhe aniccato …pe…",
  "patthana1.19:4.1_3": "domanassaṁ uppajjati,",
  "patthana1.19:4.1_4": "dibbena cakkhunā rūpaṁ passati,",
  "patthana1.19:4.1_5": "dibbāya sotadhātuyā saddaṁ suṇāti,",
  "patthana1.19:4.1_6": "rūpāyatanaṁ cakkhuviññāṇassa …pe…",
  "patthana1.19:4.1_7": "phoṭṭhabbāyatanaṁ kāyaviññāṇassa …pe…",
  "patthana1.19:4.1_8": "paccuppannā khandhā iddhividhañāṇassa,",
  "patthana1.19:4.1_9": "āvajjanāya ārammaṇapaccayena paccayo.",
  "patthana1.19:5.0_0": "Adhipati",
  "patthana1.19:5.1_0": "Atīto dhammo paccuppannassa dhammassa adhipatipaccayena paccayo.",
  "patthana1.19:5.1_1": "**Ārammaṇādhipati**—dānaṁ datvā sīlaṁ samādiyitvā …pe…",
  "patthana1.19:5.1_2": "pubbe suciṇṇāni garuṁ katvā paccavekkhati,",
  "patthana1.19:5.1_3": "jhānā vuṭṭhahitvā jhānaṁ garuṁ katvā paccavekkhati.",
  "patthana1.19:5.1_4": "Ariyā maggā vuṭṭhahitvā maggaṁ garuṁ katvā paccavekkhanti,",
  "patthana1.19:5.1_5": "phalaṁ garuṁ katvā paccavekkhanti.",
  "patthana1.19:5.1_6": "Atītaṁ cakkhuṁ …pe… kāyaṁ … rūpe … sadde … gandhe … rase …",
  "patthana1.19:5.1_7": "phoṭṭhabbe … vatthuṁ …",
  "patthana1.19:5.1_8": "atīte khandhe garuṁ katvā assādeti abhinandati,",
  "patthana1.19:5.1_9": "taṁ garuṁ katvā rāgo uppajjati,",
  "patthana1.19:5.1_10": "diṭṭhi uppajjati.",
  "patthana1.19:6.1_0": "Anāgato dhammo paccuppannassa dhammassa adhipatipaccayena paccayo.",
  "patthana1.19:6.1_1": "**Ārammaṇādhipati**—anāgataṁ cakkhuṁ …pe… vatthuṁ …",
  "patthana1.19:6.1_2": "anāgate khandhe garuṁ katvā assādeti abhinandati,",
  "patthana1.19:6.1_3": "taṁ garuṁ katvā rāgo uppajjati,",
  "patthana1.19:6.1_4": "diṭṭhi uppajjati.",
  "patthana1.19:7.1_0": "Paccuppanno dhammo paccuppannassa dhammassa adhipatipaccayena paccayo— ārammaṇādhipati,",
  "patthana1.19:7.1_1": "sahajātādhipati.",
  "patthana1.19:7.1_2": "**Ārammaṇādhipati**—paccuppannaṁ cakkhuṁ …pe… vatthuṁ …",
  "patthana1.19:7.1_3": "paccuppanne khandhe garuṁ katvā assādeti abhinandati,",
  "patthana1.19:7.1_4": "taṁ garuṁ katvā rāgo uppajjati,",
  "patthana1.19:7.1_5": "diṭṭhi uppajjati.",
  "patthana1.19:7.1_6": "**Sahajātādhipati**—paccuppannādhipati sampayuttakānaṁ khandhānaṁ cittasamuṭṭhānānañca rūpānaṁ adhipatipaccayena paccayo.",
  "patthana1.19:8.0_0": "Anantara",
  "patthana1.19:8.1_0": "Atīto dhammo paccuppannassa dhammassa anantarapaccayena paccayo—purimā purimā atītā khandhā pacchimānaṁ",
  "patthana1.19:8.1_1": "pacchimānaṁ paccuppannānaṁ khandhānaṁ anantarapaccayena paccayo.",
  "patthana1.19:8.1_2": "Anulomaṁ gotrabhussa …",
  "patthana1.19:8.1_3": "anulomaṁ vodānassa …",
  "patthana1.19:8.1_4": "gotrabhu maggassa …",
  "patthana1.19:8.1_5": "vodānaṁ maggassa …",
  "patthana1.19:8.1_6": "maggo phalassa …",
  "patthana1.19:8.1_7": "phalaṁ phalassa …",
  "patthana1.19:8.1_8": "anulomaṁ phalasamāpattiyā …",
  "patthana1.19:8.1_9": "nirodhā vuṭṭhahantassa nevasaññānāsaññāyatanaṁ phalasamāpattiyā anantarapaccayena paccayo.",
  "patthana1.19:9.0_0": "Samanantara",
  "patthana1.19:9.1_0": "Atīto dhammo paccuppannassa dhammassa samanantarapaccayena paccayo. (Anantarasadisaṁ.)",
  "patthana1.19:10.0_0": "Sahajātādi",
  "patthana1.19:10.1_0": "Paccuppanno dhammo paccuppannassa dhammassa sahajātapaccayena paccayo …",
  "patthana1.19:10.1_1": "aññamaññapaccayena paccayo …",
  "patthana1.19:10.1_2": "nissayapaccayena paccayo.",
  "patthana1.19:10.1_3": "(Saṅkhittaṁ.)",
  "patthana1.19:11.0_0": "Upanissaya",
  "patthana1.19:11.1_0": "Atīto dhammo paccuppannassa dhammassa upanissayapaccayena paccayo— ārammaṇūpanissayo,",
  "patthana1.19:11.1_1": "anantarūpanissayo,",
  "patthana1.19:11.1_2": "pakatūpanissayo …pe….",
  "patthana1.19:11.1_3": "**Pakatūpanissayo**—atītaṁ saddhaṁ upanissāya dānaṁ deti,",
  "patthana1.19:11.1_4": "sīlaṁ samādiyati,",
  "patthana1.19:11.1_5": "uposathakammaṁ karoti,",
  "patthana1.19:11.1_6": "jhānaṁ uppādeti,",
  "patthana1.19:11.1_7": "vipassanaṁ … maggaṁ …",
  "patthana1.19:11.1_8": "abhiññaṁ …",
  "patthana1.19:11.1_9": "samāpattiṁ uppādeti,",
  "patthana1.19:11.1_10": "mānaṁ jappeti,",
  "patthana1.19:11.1_11": "diṭṭhiṁ gaṇhāti.",
  "patthana1.19:11.1_12": "Atītaṁ sīlaṁ …pe… paññaṁ …",
  "patthana1.19:11.1_13": "rāgaṁ …pe…",
  "patthana1.19:11.1_14": "patthanaṁ …",
  "patthana1.19:11.1_15": "kāyikaṁ sukhaṁ …",
  "patthana1.19:11.1_16": "kāyikaṁ dukkhaṁ upanissāya dānaṁ deti sīlaṁ samādiyati,",
  "patthana1.19:11.1_17": "uposathakammaṁ …pe…",
  "patthana1.19:11.1_18": "samāpattiṁ uppādeti,",
  "patthana1.19:11.1_19": "pāṇaṁ hanati …pe…",
  "patthana1.19:11.1_20": "saṅghaṁ bhindati.",
  "patthana1.19:11.1_21": "Atītā saddhā …pe… paññā, rāgo …pe… patthanā,",
  "patthana1.19:11.1_22": "kāyikaṁ sukhaṁ …",
  "patthana1.19:11.1_23": "kāyikaṁ dukkhaṁ …",
  "patthana1.19:11.1_24": "paccuppannāya saddhāya …pe… paññāya,",
  "patthana1.19:11.1_25": "rāgassa …pe…",
  "patthana1.19:11.1_26": "patthanāya …pe…",
  "patthana1.19:11.1_27": "phalasamāpattiyā upanissayapaccayena paccayo.",
  "patthana1.19:12.1_0": "Anāgato dhammo paccuppannassa dhammassa upanissayapaccayena paccayo— ārammaṇūpanissayo,",
  "patthana1.19:12.1_1": "pakatūpanissayo …pe….",
  "patthana1.19:12.1_2": "**Pakatūpanissayo**—anāgataṁ cakkhusampadaṁ patthayamāno …pe…",
  "patthana1.19:12.1_3": "sotasampadaṁ …",
  "patthana1.19:12.1_4": "ghānasampadaṁ jivhāsampadaṁ …",
  "patthana1.19:12.1_5": "kāyasampadaṁ …",
  "patthana1.19:12.1_6": "vaṇṇasampadaṁ …",
  "patthana1.19:12.1_7": "saddasampadaṁ …",
  "patthana1.19:12.1_8": "gandhasampadaṁ …",
  "patthana1.19:12.1_9": "rasasampadaṁ …",
  "patthana1.19:12.1_10": "phoṭṭhabbasampadaṁ …",
  "patthana1.19:12.1_11": "patthayamāno …pe…",
  "patthana1.19:12.1_12": "anāgate khandhe patthayamāno dānaṁ deti,",
  "patthana1.19:12.1_13": "sīlaṁ samādiyati,",
  "patthana1.19:12.1_14": "uposathakammaṁ …",
  "patthana1.19:12.1_15": "anāgatā cakkhusampadā …pe…",
  "patthana1.19:12.1_16": "vaṇṇasampadā …pe…",
  "patthana1.19:12.1_17": "phoṭṭhabbasampadā …",
  "patthana1.19:12.1_18": "anāgatā khandhā paccuppannāya saddhāya …pe… paññāya,",
  "patthana1.19:12.1_19": "kāyikassa sukhassa,",
  "patthana1.19:12.1_20": "kāyikassa dukkhassa, maggassa,",
  "patthana1.19:12.1_21": "phalasamāpattiyā upanissayapaccayena paccayo.",
  "patthana1.19:13.1_0": "Paccuppanno dhammo paccuppannassa dhammassa upanissayapaccayena paccayo— ārammaṇūpanissayo,",
  "patthana1.19:13.1_1": "pakatūpanissayo …pe….",
  "patthana1.19:13.1_2": "**Pakatūpanissayo**—paccuppannaṁ utuṁ upanissāya jhānaṁ uppādeti,",
  "patthana1.19:13.1_3": "vipassanaṁ …pe…",
  "patthana1.19:13.1_4": "paccuppannaṁ bhojanaṁ senāsanaṁ upanissāya jhānaṁ uppādeti …pe…",
  "patthana1.19:13.1_5": "samāpattiṁ uppādeti.",
  "patthana1.19:13.1_6": "Paccuppannaṁ utu …",
  "patthana1.19:13.1_7": "bhojanaṁ …",
  "patthana1.19:13.1_8": "senāsanaṁ paccuppannāya saddhāya …pe…",
  "patthana1.19:13.1_9": "paññāya kāyikassa …pe…",
  "patthana1.19:13.1_10": "phalasamāpattiyā upanissayapaccayena paccayo.",
  "patthana1.19:14.0_0": "Purejāta",
  "patthana1.19:14.1_0": "Paccuppanno dhammo paccuppannassa dhammassa purejātapaccayena paccayo— ārammaṇapurejātaṁ,",
  "patthana1.19:14.1_1": "vatthupurejātaṁ.",
  "patthana1.19:14.1_2": "**Ārammaṇapurejātaṁ**—cakkhuṁ …pe…",
  "patthana1.19:14.1_3": "vatthuṁ aniccato …pe…",
  "patthana1.19:14.1_4": "domanassaṁ uppajjati,",
  "patthana1.19:14.1_5": "dibbena cakkhunā rūpaṁ passati,",
  "patthana1.19:14.1_6": "dibbāya sotadhātuyā saddaṁ suṇāti,",
  "patthana1.19:14.1_7": "rūpāyatanaṁ cakkhuviññāṇassa …pe…",
  "patthana1.19:14.1_8": "phoṭṭhabbāyatanaṁ kāyaviññāṇassa purejātapaccayena paccayo.",
  "patthana1.19:14.1_9": "**Vatthupurejātaṁ**—cakkhāyatanaṁ cakkhuviññāṇassa …pe…",
  "patthana1.19:14.1_10": "kāyāyatanaṁ kāyaviññāṇassa …",
  "patthana1.19:14.1_11": "vatthu paccuppannānaṁ khandhānaṁ purejātapaccayena paccayo.",
  "patthana1.19:15.0_0": "Pacchājāta",
  "patthana1.19:15.1_0": "Paccuppanno dhammo paccuppannassa dhammassa pacchājātapaccayena paccayo— pacchājātā paccuppannā khandhā",
  "patthana1.19:15.1_1": "purejātassa imassa kāyassa pacchājātapaccayena paccayo.",
  "patthana1.19:16.0_0": "Āsevana",
  "patthana1.19:16.1_0": "Atīto dhammo paccuppannassa dhammassa āsevanapaccayena paccayo—purimā purimā atītā khandhā pacchimānaṁ",
  "patthana1.19:16.1_1": "pacchimānaṁ paccuppannānaṁ khandhānaṁ āsevanapaccayena paccayo.",
  "patthana1.19:16.1_2": "Anulomaṁ gotrabhussa …",
  "patthana1.19:16.1_3": "anulomaṁ vodānassa …",
  "patthana1.19:16.1_4": "gotrabhu maggassa …",
  "patthana1.19:16.1_5": "vodānaṁ maggassa āsevanapaccayena paccayo.",
  "patthana1.19:17.0_0": "Kamma",
  "patthana1.19:17.1_0": "Atīto dhammo paccuppannassa dhammassa kammapaccayena paccayo.",
  "patthana1.19:17.1_1": "**Nānākkhaṇikā**—atītā cetanā paccuppannānaṁ vipākānaṁ khandhānaṁ kaṭattā ca rūpānaṁ kammapaccayena paccayo.",
  "patthana1.19:18.1_0": "Paccuppanno dhammo paccuppannassa dhammassa kammapaccayena paccayo— paccuppannā cetanā sampayuttakānaṁ",
  "patthana1.19:18.1_1": "khandhānaṁ cittasamuṭṭhānānañca rūpānaṁ kammapaccayena paccayo.",
  "patthana1.19:18.1_2": "Paṭisandhikkhaṇe paccuppannā cetanā sampayuttakānaṁ khandhānaṁ kaṭattā ca rūpānaṁ kammapaccayena paccayo.",
  "patthana1.19:19.0_0": "Vipāka",
  "patthana1.19:19.1_0": "Paccuppanno dhammo paccuppannassa dhammassa vipākapaccayena paccayo—vipāko paccuppanno eko khandho tiṇṇannaṁ",
  "patthana1.19:19.1_1": "khandhānaṁ cittasamuṭṭhānānañca rūpānaṁ vipākapaccayena paccayo …pe…",
  "patthana1.19:19.1_2": "dve khandhā …pe…",
  "patthana1.19:19.1_3": "paṭisandhikkhaṇe …pe…",
  "patthana1.19:19.1_4": "khandhā vatthussa vipākapaccayena paccayo.",
  "patthana1.19:20.0_0": "Āhārādi",
  "patthana1.19:20.1_0": "Paccuppanno dhammo paccuppannassa dhammassa āhārapaccayena paccayo …",
  "patthana1.19:20.1_1": "indriyapaccayena paccayo …",
  "patthana1.19:20.1_2": "jhānapaccayena paccayo …",
  "patthana1.19:20.1_3": "maggapaccayena paccayo …",
  "patthana1.19:20.1_4": "sampayuttapaccayena paccayo …",
  "patthana1.19:20.1_5": "vippayuttapaccayena paccayo—sahajātaṁ,",
  "patthana1.19:20.1_6": "purejātaṁ,",
  "patthana1.19:20.1_7": "pacchājātaṁ.",
  "patthana1.19:20.1_8": "**Sahajātā**—paccuppannā khandhā cittasamuṭṭhānānaṁ rūpānaṁ vippayuttapaccayena paccayo.",
  "patthana1.19:20.1_9": "Paṭisandhikkhaṇe paccuppannā khandhā kaṭattārūpānaṁ vippayuttapaccayena paccayo,",
  "patthana1.19:20.1_10": "khandhā vatthussa vippayuttapaccayena paccayo,",
  "patthana1.19:20.1_11": "vatthu khandhānaṁ vippayuttapaccayena paccayo.",
  "patthana1.19:20.1_12": "**Purejātaṁ**—cakkhāyatanaṁ cakkhuviññāṇassa …pe…",
  "patthana1.19:20.1_13": "kāyāyatanaṁ kāyaviññāṇassa …",
  "patthana1.19:20.1_14": "vatthu paccuppannānaṁ khandhānaṁ vippayuttapaccayena paccayo.",
  "patthana1.19:20.1_15": "**Pacchājātā**—paccuppannā khandhā purejātassa imassa kāyassa vippayuttapaccayena paccayo.",
  "patthana1.19:21.0_0": "Atthi",
  "patthana1.19:21.1_0": "Paccuppanno dhammo paccuppannassa dhammassa atthipaccayena paccayo. (Uppannattike atthisadisaṁ.)",
  "patthana1.19:22.0_0": "Natthi-vigatāvigata",
  "patthana1.19:22.1_0": "Atīto dhammo paccuppannassa dhammassa natthipaccayena paccayo … vigatapaccayena paccayo.",
  "patthana1.19:23.1_0": "Paccuppanno dhammo paccuppannassa dhammassa avigatapaccayena paccayo …pe….",
  "patthana1.19:24.0.1_0": "Saṅkhyāvāra",
  "patthana1.19:24.0.2_0": "Suddha",
  "patthana1.19:24.1_0": "Hetuyā ekaṁ,",
  "patthana1.19:24.1_1": "ārammaṇe tīṇi,",
  "patthana1.19:24.1_2": "adhipatiyā tīṇi,",
  "patthana1.19:24.1_3": "anantare ekaṁ,",
  "patthana1.19:24.1_4": "samanantare ekaṁ,",
  "patthana1.19:24.1_5": "sahajāte aññamaññe nissaye ekaṁ,",
  "patthana1.19:24.1_6": "upanissaye tīṇi,",
  "patthana1.19:24.1_7": "purejāte pacchājāte āsevane ekaṁ,",
  "patthana1.19:24.1_8": "kamme dve,",
  "patthana1.19:24.1_9": "vipāke āhāre ekaṁ …pe…",
  "patthana1.19:24.1_10": "avigate ekaṁ.",
  "patthana1.19:24.1_11": "(Evaṁ gaṇetabbaṁ.)",
  "patthana1.19:25.1_0": "Anulomaṁ.",
  "patthana1.19:26.0_0": "Paccanīyuddhāra",
  "patthana1.19:26.1_0": "Atīto dhammo paccuppannassa dhammassa ārammaṇapaccayena paccayo …",
  "patthana1.19:26.1_1": "upanissayapaccayena paccayo …",
  "patthana1.19:26.1_2": "kammapaccayena paccayo.",
  "patthana1.19:27.1_0": "Anāgato dhammo paccuppannassa dhammassa ārammaṇapaccayena paccayo … upanissayapaccayena paccayo.",
  "patthana1.19:28.1_0": "Paccuppanno dhammo paccuppannassa dhammassa ārammaṇapaccayena paccayo …",
  "patthana1.19:28.1_1": "sahajātapaccayena paccayo …",
  "patthana1.19:28.1_2": "upanissayapaccayena paccayo …",
  "patthana1.19:28.1_3": "purejātapaccayena paccayo …",
  "patthana1.19:28.1_4": "pacchājātapaccayena paccayo …",
  "patthana1.19:28.1_5": "āhārapaccayena paccayo …",
  "patthana1.19:28.1_6": "indriyapaccayena paccayo.",
  "patthana1.19:29.0.1_0": "1.19.7.2. Paccayapaccanīya",
  "patthana1.19:29.0.2_0": "Saṅkhyāvāra",
  "patthana1.19:29.0.3_0": "Suddha",
  "patthana1.19:29.1_0": "Nahetuyā tīṇi,",
  "patthana1.19:29.1_1": "naārammaṇe tīṇi,",
  "patthana1.19:29.1_2": "naadhipatiyā tīṇi,",
  "patthana1.19:29.1_3": "naanantare tīṇi …pe…",
  "patthana1.19:29.1_4": "nasampayutte tīṇi,",
  "patthana1.19:29.1_5": "navippayutte tīṇi,",
  "patthana1.19:29.1_6": "noatthiyā dve,",
  "patthana1.19:29.1_7": "nonatthiyā tīṇi,",
  "patthana1.19:29.1_8": "novigate tīṇi,",
  "patthana1.19:29.1_9": "noavigate dve.",
  "patthana1.19:29.1_10": "(Evaṁ gaṇetabbaṁ.)",
  "patthana1.19:30.1_0": "Paccanīyaṁ.",
  "patthana1.19:31.0_0": "1.19.7.3. Paccayānulomapaccanīya",
  "patthana1.19:31.1_0": "Hetupaccayā naārammaṇe ekaṁ,",
  "patthana1.19:31.1_1": "naadhipatiyā naanantare nasamanantare naaññamaññe naupanissaye …pe…",
  "patthana1.19:31.1_2": "nasampayutte navippayutte nonatthiyā novigate ekaṁ.",
  "patthana1.19:31.1_3": "(Evaṁ gaṇetabbaṁ.)",
  "patthana1.19:32.1_0": "Anulomapaccanīyaṁ.",
  "patthana1.19:33.0_0": "1.19.7.4. Paccayapaccanīyānuloma",
  "patthana1.19:33.1_0": "Nahetupaccayā ārammaṇe tīṇi,",
  "patthana1.19:33.1_1": "adhipatiyā tīṇi,",
  "patthana1.19:33.1_2": "anantare samanantare sahajāte aññamaññe nissaye ekaṁ,",
  "patthana1.19:33.1_3": "upanissaye tīṇi,",
  "patthana1.19:33.1_4": "purejāte ekaṁ,",
  "patthana1.19:33.1_5": "pacchājāte āsevane ekaṁ …pe…",
  "patthana1.19:33.1_6": "kamme dve,",
  "patthana1.19:33.1_7": "vipāke ekaṁ (imesu padesu ekaṁyeva),",
  "patthana1.19:33.1_8": "avigate ekaṁ.",
  "patthana1.19:33.1_9": "(Evaṁ gaṇetabbaṁ.)",
  "patthana1.19:34.1_0": "Paccanīyānulomaṁ.",
  "patthana1.19:34.2_0": "Pañhāvāro.",
  "patthana1.19:35.1_0": "Atītattikaṁ niṭṭhitaṁ."
}