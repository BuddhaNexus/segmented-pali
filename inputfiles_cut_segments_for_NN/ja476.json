{
  "ja476:0.1_0": "Jātaka",
  "ja476:0.2_0": "Terasakanipāta",
  "ja476:0.3_0": "Ambavagga",
  "ja476:0.4_0": "3. Javanahaṁsajātaka",
  "ja476:1.1_0": "“Idheva haṁsa nipata,",
  "ja476:1.2_0": "piyaṁ me tava dassanaṁ;",
  "ja476:1.3_0": "Issarosi anuppatto,",
  "ja476:1.4_0": "yamidhatthi pavedaya”.",
  "ja476:2.1_0": "“Savanena ekassa piyā bhavanti,",
  "ja476:2.2_0": "Disvā panekassa viyeti chando;",
  "ja476:2.3_0": "Disvā ca sutvā ca piyā bhavanti,",
  "ja476:2.4_0": "Kaccinnu me pīyasi dassanena.",
  "ja476:3.1_0": "Savanena piyo mesi,",
  "ja476:3.2_0": "bhiyyo cāgamma dassanaṁ;",
  "ja476:3.3_0": "Evaṁ piyadassano me,",
  "ja476:3.4_0": "vasa haṁsa mamantike”.",
  "ja476:4.1_0": "“Vaseyyāma tavāgāre,",
  "ja476:4.2_0": "niccaṁ sakkatapūjitā;",
  "ja476:4.3_0": "Matto ca ekadā vajje,",
  "ja476:4.4_0": "‘haṁsarājaṁ pacantu me’”.",
  "ja476:5.1_0": "“Dhiratthu taṁ majjapānaṁ,",
  "ja476:5.2_0": "yaṁ me piyataraṁ tayā;",
  "ja476:5.3_0": "Na cāpi majjaṁ pissāmi,",
  "ja476:5.4_0": "yāva me vacchasī ghare”.",
  "ja476:6.1_0": "“Suvijānaṁ siṅgālānaṁ,",
  "ja476:6.2_0": "sakuṇānañca vassitaṁ;",
  "ja476:6.3_0": "Manussavassitaṁ rāja,",
  "ja476:6.4_0": "dubbijānataraṁ tato.",
  "ja476:7.1_0": "Api ce maññatī poso,",
  "ja476:7.2_0": "ñāti mitto sakhāti vā;",
  "ja476:7.3_0": "Yo pubbe sumano hutvā,",
  "ja476:7.4_0": "pacchā sampajjate diso.",
  "ja476:8.1_0": "Yasmiṁ mano nivisati,",
  "ja476:8.2_0": "avidūre sahāpi so;",
  "ja476:8.3_0": "Santikepi hi so dūre,",
  "ja476:8.4_0": "yasmiṁ nāvisate mano.",
  "ja476:9.1_0": "Antopi so hoti pasannacitto,",
  "ja476:9.2_0": "Pāraṁ samuddassa pasannacitto;",
  "ja476:9.3_0": "Antopi so hoti paduṭṭhacitto,",
  "ja476:9.4_0": "Pāraṁ samuddassa paduṭṭhacitto.",
  "ja476:10.1_0": "Saṁvasantā vivasanti,",
  "ja476:10.2_0": "ye disā te rathesabha;",
  "ja476:10.3_0": "Ārā santo saṁvasanti,",
  "ja476:10.4_0": "manasā raṭṭhavaḍḍhana.",
  "ja476:11.1_0": "Aticiraṁ nivāsena,",
  "ja476:11.2_0": "piyo bhavati appiyo;",
  "ja476:11.3_0": "Āmanta kho taṁ gacchāma,",
  "ja476:11.4_0": "purā te homa appiyā”.",
  "ja476:12.1_0": "“Evañce yācamānānaṁ,",
  "ja476:12.2_0": "Añjaliṁ nāvabujjhasi;",
  "ja476:12.3_0": "Paricārakānaṁ sataṁ,",
  "ja476:12.4_0": "Vacanaṁ na karosi no;",
  "ja476:12.5_0": "Evaṁ taṁ abhiyācāma,",
  "ja476:12.6_0": "Puna kayirāsi pariyāyaṁ”.",
  "ja476:13.1_0": "“Evañce no viharataṁ,",
  "ja476:13.2_0": "antarāyo na hessati;",
  "ja476:13.3_0": "Tuyhañcāpi mahārāja,",
  "ja476:13.4_0": "mayhañca raṭṭhavaḍḍhana;",
  "ja476:13.5_0": "Appeva nāma passemu,",
  "ja476:13.6_0": "ahorattānamaccaye”ti.",
  "ja476:14.1_0": "Javanahaṁsajātakaṁ tatiyaṁ."
}