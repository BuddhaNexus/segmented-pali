{
  "ne35:0.1_0": "Netti",
  "ne35:0.2_0": "Paṭiniddesavāra",
  "ne35:0.3_0": "Sampāta 16",
  "ne35:0.4_0": "Samāropanahārasampāta",
  "ne35:1.1_0": "Tattha katamo samāropano hārasampāto?",
  "ne35:2.1_0": "“Tasmā rakkhitacittassa,",
  "ne35:2.2_0": "sammāsaṅkappagocaro;",
  "ne35:2.3_0": "Sammādiṭṭhipurekkhāro,",
  "ne35:2.4_0": "ñatvāna udayabbayaṁ;",
  "ne35:2.5_0": "Thinamiddhābhibhū bhikkhu,",
  "ne35:2.6_0": "sabbā duggatiyo jahe”ti.",
  "ne35:3.1_0": "“Tasmā rakkhitacittassā”ti tiṇṇaṁ sucaritānaṁ padaṭṭhānaṁ,",
  "ne35:3.1_1": "citte rakkhite taṁ rakkhitaṁ bhavati kāyakammaṁ vacīkammaṁ manokammaṁ.",
  "ne35:3.2_0": "Sammādiṭṭhipurekkhāroti sammādiṭṭhiyā bhāvitāya bhāvito bhavati ariyo aṭṭhaṅgiko maggo.",
  "ne35:3.3_0": "Kena kāraṇena?",
  "ne35:3.4_0": "Sammādiṭṭhito hi sammāsaṅkappo pabhavati,",
  "ne35:3.4_1": "sammāsaṅkappato sammāvācā pabhavati,",
  "ne35:3.4_2": "sammāvācāto sammākammanto pabhavati,",
  "ne35:3.4_3": "sammākammantato sammāājīvo pabhavati,",
  "ne35:3.4_4": "sammāājīvato sammāvāyāmo pabhavati,",
  "ne35:3.4_5": "sammāvāyāmato sammāsati pabhavati,",
  "ne35:3.4_6": "sammāsatito sammāsamādhi pabhavati,",
  "ne35:3.4_7": "sammāsamādhito sammāvimutti pabhavati,",
  "ne35:3.4_8": "sammāvimuttito sammāvimuttiñāṇadassanaṁ pabhavati.",
  "ne35:3.5_0": "Ayaṁ anupādiseso puggalo anupādisesā ca nibbānadhātu.",
  "ne35:3.6_0": "Niyutto samāropano hārasampāto.",
  "ne35:4.1_0": "Tenāha āyasmā mahākaccāyano—",
  "ne35:5.1_0": "“Soḷasa hārā paṭhamaṁ,",
  "ne35:5.2_0": "Disalocanato disā viloketvā;",
  "ne35:5.3_0": "Saṅkhipiya aṅkusena hi,",
  "ne35:5.4_0": "Nayehi tīhi niddise suttan”ti.",
  "ne35:5.5_0": "Niyutto hārasampāto."
}