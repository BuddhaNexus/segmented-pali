{
  "tika-vin11t41:0_0": "Khandhakapucchākathāvaṇṇanā",
  "tika-vin11t41:1_0": "300.Sesesūti abhabbapuggalaparidīpakesu sabbapadesu.",
  "tika-vin11t41:2_0": "302.",
  "tika-vin11t41:2_1": "“Nassantu ete”ti padacchedo.",
  "tika-vin11t41:2_2": "Purakkhakāti ettha sāmiatthe paccattavacanaṁ,",
  "tika-vin11t41:2_3": "bhedapurekkhakassa,",
  "tika-vin11t41:2_4": "bhedapurekkhakāyāti attho.",
  "tika-vin11t41:3_0": "303.Sesesūti avasesesu asaṁvāsakādidīpakesu paṭikkhepapadesu.",
  "tika-vin11t41:4_0": "304.Ekāvadukkaṭāpatti vuttāti vassaṁ anupagamanādipaccayā jātito ekāva dukkaṭāpatti vuttā.",
  "tika-vin11t41:5_0": "305.Uposathasamāmatāti uposathakkhandhake vuttasadisā jātā āpattiyo matā adhippetā.",
  "tika-vin11t41:6_0": "306.Cammeti cammakkhandhake.",
  "tika-vin11t41:6_1": "Vacchatariṁ gahetvā mārentānaṁ chabbaggiyānaṁ pācitti vuttāti sambandho.",
  "tika-vin11t41:6_2": "Vacchatarinti balasampannaṁ taruṇagāviṁ.",
  "tika-vin11t41:6_3": "Sā hi vacchakabhāvaṁ taritvā atikkamitvā ṭhitattā “vacchatarī”ti vuccati.",
  "tika-vin11t41:7_0": "307.Aṅgajātaṁ chupantassāti gāvīnaṁ aṅgajātaṁ attano aṅgajātena bahi chupantassa.",
  "tika-vin11t41:7_1": "Sesesūti gāvīnaṁ visāṇādīsu gahaṇe,",
  "tika-vin11t41:7_2": "piṭṭhiabhiruhaṇe ca.",
  "tika-vin11t41:7_3": "Yathāha “chabbaggiyā bhikkhū aciravatiyā nadiyā gāvīnaṁ tarantīnaṁ visāṇesupi gaṇhantī”tiādi.",
  "tika-vin11t41:8_0": "309.Tattha bhesajjakkhandhake.",
  "tika-vin11t41:8_1": "Sāmantā dvaṅguleti vaccamaggapassāvamaggānaṁ sāmantā dvaṅgulamatte padese.",
  "tika-vin11t41:8_2": "Satthakammaṁ karontassa thullaccayamudīritanti yojanā. Yathāha – “na,",
  "tika-vin11t41:8_3": "bhikkhave,",
  "tika-vin11t41:8_4": "sambādhassa sāmantā dvaṅgule satthakammaṁ vā vatthikammaṁ vā kāretabbaṁ,",
  "tika-vin11t41:8_5": "yo kāreyya,",
  "tika-vin11t41:8_6": "āpatti thullaccayassā”ti.",
  "tika-vin11t41:8_7": "Ettha ca “sāmantā dvaṅgule”ti idaṁ satthakammaṁyeva sandhāya vuttaṁ.",
  "tika-vin11t41:8_8": "Vatthikammaṁ pana sambādheyeva paṭikkhittaṁ.",
  "tika-vin11t41:9_0": "“Na,",
  "tika-vin11t41:9_1": "bhikkhave,",
  "tika-vin11t41:9_2": "aññatra nimantitena aññassa bhojjayāgu paribhuñjitabbā,",
  "tika-vin11t41:9_3": "yo paribhuñjeyya,",
  "tika-vin11t41:9_4": "yathādhammo kāretabbo”ti vuttattā āha “bhojjayāgūsu pācittī”ti.",
  "tika-vin11t41:9_5": "Ettha ca bhojjayāgu nāma bahalayāgu.",
  "tika-vin11t41:9_6": "“Piṇḍaṁ vaṭṭetvā pātabbayāgū”ti gaṇṭhipade vuttaṁ.",
  "tika-vin11t41:9_7": "Pācittīti paramparabhojanapavāraṇasikkhāpadehi pācitti.",
  "tika-vin11t41:9_8": "Sesesūti antovutthaantopakkasayaṁpakkaparibhogādīsu.",
  "tika-vin11t41:9_9": "Yathāha “na,",
  "tika-vin11t41:9_10": "bhikkhave,",
  "tika-vin11t41:9_11": "antovutthaṁ antopakkaṁ sāmaṁpakkaṁ paribhuñjitabbaṁ,",
  "tika-vin11t41:9_12": "yo paribhuñjeyya,",
  "tika-vin11t41:9_13": "āpatti dukkaṭassā”tiādi.",
  "tika-vin11t41:10_0": "310.Cīvarasaṁyutteti cīvarakkhandhake.",
  "tika-vin11t41:11_0": "313.Campeyyakeca kosambeti campeyyakkhandhake ceva kosambakakkhandhake ca.",
  "tika-vin11t41:11_1": "“Kammasmi”ntiādīsupi eseva nayo.",
  "tika-vin11t41:12_0": "317.Romantheti bhuttassa lahuṁ pākattāya kucchigataṁ mukhaṁ āropetvā saṇhakaraṇavasena anucālane.",
  "tika-vin11t41:13_0": "318.Senāsanasminti senāsanakkhandhake. Garunoti garubhaṇḍassa.",
  "tika-vin11t41:14_0": "320.Saṅghabhedeti saṅghabhedakakkhandhake.",
  "tika-vin11t41:15_0": "321.Bhedānuvattakānanti saṅghabhedānuvattakānaṁ. Gaṇabhogeti gaṇabhojane.",
  "tika-vin11t41:16_0": "322.Sāti ettha sabbavattesu anādariyena hotīti seso. Sesaṁ uttānatthameva.",
  "tika-vin11t41:17_0": "Iti uttare līnatthapakāsaniyā",
  "tika-vin11t41:18_0": "Khandhakapucchākathāvaṇṇanā niṭṭhitā."
}