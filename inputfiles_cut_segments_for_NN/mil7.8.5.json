{
  "mil7.8.5:0.1_0": "Milindapañha",
  "mil7.8.5:0.2_0": "Opammakathāpañha",
  "mil7.8.5:0.3_0": "Kumbhavagga",
  "mil7.8.5:0.4_0": "5. Agadaṅgapañha",
  "mil7.8.5:1.1_0": "“Bhante nāgasena,",
  "mil7.8.5:1.1_1": "‘agadassa dve aṅgāni gahetabbānī’ti yaṁ vadesi,",
  "mil7.8.5:1.1_2": "katamāni tāni dve aṅgāni gahetabbānī”ti.",
  "mil7.8.5:1.2_0": "“Yathā, mahārāja, agade kimī na saṇṭhahanti;",
  "mil7.8.5:1.3_0": "evameva kho, mahārāja,",
  "mil7.8.5:1.3_1": "yoginā yogāvacarena mānase kilesā na saṇṭhapetabbā.",
  "mil7.8.5:1.4_0": "Idaṁ, mahārāja,",
  "mil7.8.5:1.4_1": "agadassa paṭhamaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.8.5:2.1_0": "Puna caparaṁ, mahārāja,",
  "mil7.8.5:2.1_1": "agado daṭṭhaphuṭṭhadiṭṭhaasitapītakhāyitasāyitaṁ sabbaṁ visaṁ paṭihanati;",
  "mil7.8.5:2.2_0": "evameva kho, mahārāja,",
  "mil7.8.5:2.2_1": "yoginā yogāvacarena rāgadosamohamānadiṭṭhivisaṁ sabbaṁ paṭihanitabbaṁ.",
  "mil7.8.5:2.3_0": "Idaṁ, mahārāja, agadassa dutiyaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.8.5:2.4_0": "Bhāsitampetaṁ, mahārāja, bhagavatā devātidevena—",
  "mil7.8.5:3.1_0": "‘Saṅkhārānaṁ sabhāvatthaṁ,",
  "mil7.8.5:3.2_0": "daṭṭhukāmena yoginā;",
  "mil7.8.5:3.3_0": "Agadeneva hotabbaṁ,",
  "mil7.8.5:3.4_0": "kilesavisanāsane’”ti.",
  "mil7.8.5:4.1_0": "Agadaṅgapañho pañcamo."
}