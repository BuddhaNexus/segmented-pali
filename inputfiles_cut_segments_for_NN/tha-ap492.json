{
  "tha-ap492:0.1_0": "Therāpadāna",
  "tha-ap492:0.2_0": "Paṁsukūlavagga",
  "tha-ap492:0.3_0": "10. Sālamaṇḍapiyattheraapadāna",
  "tha-ap492:1.1_0": "“Ajjhogāhetvā sālavanaṁ,",
  "tha-ap492:1.2_0": "sukato assamo mama;",
  "tha-ap492:1.3_0": "Sālapupphehi sañchanno,",
  "tha-ap492:1.4_0": "vasāmi vipine tadā.",
  "tha-ap492:2.1_0": "Piyadassī ca bhagavā,",
  "tha-ap492:2.2_0": "sayambhū aggapuggalo;",
  "tha-ap492:2.3_0": "Vivekakāmo sambuddho,",
  "tha-ap492:2.4_0": "sālavanamupāgami.",
  "tha-ap492:3.1_0": "Assamā abhinikkhamma,",
  "tha-ap492:3.2_0": "pavanaṁ agamāsahaṁ;",
  "tha-ap492:3.3_0": "Mūlaphalaṁ gavesanto,",
  "tha-ap492:3.4_0": "āhiṇḍāmi vane tadā.",
  "tha-ap492:4.1_0": "Tatthaddasāsiṁ sambuddhaṁ,",
  "tha-ap492:4.2_0": "piyadassiṁ mahāyasaṁ;",
  "tha-ap492:4.3_0": "Sunisinnaṁ samāpannaṁ,",
  "tha-ap492:4.4_0": "virocantaṁ mahāvane.",
  "tha-ap492:5.1_0": "Catudaṇḍe ṭhapetvāna,",
  "tha-ap492:5.2_0": "buddhassa uparī ahaṁ;",
  "tha-ap492:5.3_0": "Maṇḍapaṁ sukataṁ katvā,",
  "tha-ap492:5.4_0": "sālapupphehi chādayiṁ.",
  "tha-ap492:6.1_0": "Sattāhaṁ dhārayitvāna,",
  "tha-ap492:6.2_0": "maṇḍapaṁ sālachāditaṁ;",
  "tha-ap492:6.3_0": "Tattha cittaṁ pasādetvā,",
  "tha-ap492:6.4_0": "buddhaseṭṭhamavandahaṁ.",
  "tha-ap492:7.1_0": "Bhagavā tamhi samaye,",
  "tha-ap492:7.2_0": "vuṭṭhahitvā samādhito;",
  "tha-ap492:7.3_0": "Yugamattaṁ pekkhamāno,",
  "tha-ap492:7.4_0": "nisīdi purisuttamo.",
  "tha-ap492:8.1_0": "Sāvako varuṇo nāma,",
  "tha-ap492:8.2_0": "piyadassissa satthuno;",
  "tha-ap492:8.3_0": "Vasīsatasahassehi,",
  "tha-ap492:8.4_0": "upagacchi vināyakaṁ.",
  "tha-ap492:9.1_0": "Piyadassī ca bhagavā,",
  "tha-ap492:9.2_0": "lokajeṭṭho narāsabho;",
  "tha-ap492:9.3_0": "Bhikkhusaṅghe nisīditvā,",
  "tha-ap492:9.4_0": "sitaṁ pātukarī jino.",
  "tha-ap492:10.1_0": "Anuruddho upaṭṭhāko,",
  "tha-ap492:10.2_0": "piyadassissa satthuno;",
  "tha-ap492:10.3_0": "Ekaṁsaṁ cīvaraṁ katvā,",
  "tha-ap492:10.4_0": "apucchittha mahāmuniṁ.",
  "tha-ap492:11.1_0": "‘Ko nu kho bhagavā hetu,",
  "tha-ap492:11.2_0": "sitakammassa satthuno;",
  "tha-ap492:11.3_0": "Kāraṇe vijjamānamhi,",
  "tha-ap492:11.4_0": "satthā pātukare sitaṁ’.",
  "tha-ap492:12.1_0": "‘Sattāhaṁ sālacchadanaṁ,",
  "tha-ap492:12.2_0": "yo me dhāresi māṇavo;",
  "tha-ap492:12.3_0": "Tassa kammaṁ saritvāna,",
  "tha-ap492:12.4_0": "sitaṁ pātukariṁ ahaṁ.",
  "tha-ap492:13.1_0": "Anokāsaṁ na passāmi,",
  "tha-ap492:13.2_0": "yattha puññaṁ vipaccati;",
  "tha-ap492:13.3_0": "Devaloke manusse vā,",
  "tha-ap492:13.4_0": "okāsova na sammati.",
  "tha-ap492:14.1_0": "Devaloke vasantassa,",
  "tha-ap492:14.2_0": "puññakammasamaṅgino;",
  "tha-ap492:14.3_0": "Yāvatā parisā tassa,",
  "tha-ap492:14.4_0": "sālacchannā bhavissati.",
  "tha-ap492:15.1_0": "Tattha dibbehi naccehi,",
  "tha-ap492:15.2_0": "gītehi vāditehi ca;",
  "tha-ap492:15.3_0": "Ramissati sadā santo,",
  "tha-ap492:15.4_0": "puññakammasamāhito.",
  "tha-ap492:16.1_0": "Yāvatā parisā tassa,",
  "tha-ap492:16.2_0": "gandhagandhī bhavissati;",
  "tha-ap492:16.3_0": "Sālassa pupphavasso ca,",
  "tha-ap492:16.4_0": "pavassissati tāvade.",
  "tha-ap492:17.1_0": "Tato cutoyaṁ manujo,",
  "tha-ap492:17.2_0": "mānusaṁ āgamissati;",
  "tha-ap492:17.3_0": "Idhāpi sālacchadanaṁ,",
  "tha-ap492:17.4_0": "sabbakālaṁ dharissati.",
  "tha-ap492:18.1_0": "Idha naccañca gītañca,",
  "tha-ap492:18.2_0": "sammatāḷasamāhitaṁ;",
  "tha-ap492:18.3_0": "Parivāressanti maṁ niccaṁ,",
  "tha-ap492:18.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap492:19.1_0": "Uggacchante ca sūriye,",
  "tha-ap492:19.2_0": "sālavassaṁ pavassati;",
  "tha-ap492:19.3_0": "Puññakammena saṁyuttaṁ,",
  "tha-ap492:19.4_0": "vassate sabbakālikaṁ.",
  "tha-ap492:20.1_0": "Aṭṭhārase kappasate,",
  "tha-ap492:20.2_0": "okkākakulasambhavo;",
  "tha-ap492:20.3_0": "Gotamo nāma gottena,",
  "tha-ap492:20.4_0": "satthā loke bhavissati.",
  "tha-ap492:21.1_0": "Tassa dhammesu dāyādo,",
  "tha-ap492:21.2_0": "oraso dhammanimmito;",
  "tha-ap492:21.3_0": "Sabbāsave pariññāya,",
  "tha-ap492:21.4_0": "nibbāyissatināsavo.",
  "tha-ap492:22.1_0": "Dhammaṁ abhisamentassa,",
  "tha-ap492:22.2_0": "sālacchannaṁ bhavissati;",
  "tha-ap492:22.3_0": "Citake jhāyamānassa,",
  "tha-ap492:22.4_0": "chadanaṁ tattha hessati’.",
  "tha-ap492:23.1_0": "Vipākaṁ kittayitvāna,",
  "tha-ap492:23.2_0": "piyadassī mahāmuni;",
  "tha-ap492:23.3_0": "Parisāya dhammaṁ desesi,",
  "tha-ap492:23.4_0": "tappento dhammavuṭṭhiyā.",
  "tha-ap492:24.1_0": "Tiṁsakappāni devesu,",
  "tha-ap492:24.2_0": "devarajjamakārayiṁ;",
  "tha-ap492:24.3_0": "Saṭṭhi ca sattakkhattuñca,",
  "tha-ap492:24.4_0": "cakkavattī ahosahaṁ.",
  "tha-ap492:25.1_0": "Devalokā idhāgantvā,",
  "tha-ap492:25.2_0": "labhāmi vipulaṁ sukhaṁ;",
  "tha-ap492:25.3_0": "Idhāpi sālacchadanaṁ,",
  "tha-ap492:25.4_0": "maṇḍapassa idaṁ phalaṁ.",
  "tha-ap492:26.1_0": "Ayaṁ pacchimako mayhaṁ,",
  "tha-ap492:26.2_0": "carimo vattate bhavo;",
  "tha-ap492:26.3_0": "Idhāpi sālacchadanaṁ,",
  "tha-ap492:26.4_0": "hessati sabbakālikaṁ.",
  "tha-ap492:27.1_0": "Mahāmuniṁ tosayitvā,",
  "tha-ap492:27.2_0": "gotamaṁ sakyapuṅgavaṁ;",
  "tha-ap492:27.3_0": "Pattomhi acalaṁ ṭhānaṁ,",
  "tha-ap492:27.4_0": "hitvā jayaparājayaṁ.",
  "tha-ap492:28.1_0": "Aṭṭhārase kappasate,",
  "tha-ap492:28.2_0": "yaṁ buddhamabhipūjayiṁ;",
  "tha-ap492:28.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap492:28.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap492:29.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap492:29.2_0": "…pe…",
  "tha-ap492:29.3_0": "viharāmi anāsavo.",
  "tha-ap492:30.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap492:30.2_0": "…pe…",
  "tha-ap492:30.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap492:31.1_0": "Paṭisambhidā catasso,",
  "tha-ap492:31.2_0": "…pe…",
  "tha-ap492:31.3_0": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap492:32.1_0": "Itthaṁ sudaṁ āyasmā sālamaṇḍapiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap492:33.1_0": "Sālamaṇḍapiyattherassāpadānaṁ dasamaṁ.",
  "tha-ap492:34.1_0": "Paṁsukūlavaggo ekūnapaññāsamo.",
  "tha-ap492:35.1_0": "Tassuddānaṁ",
  "tha-ap492:36.1_0": "Paṁsukūlaṁ buddhasaññī,",
  "tha-ap492:36.2_0": "bhisado ñāṇakittako;",
  "tha-ap492:36.3_0": "Candanī dhātupūjī ca,",
  "tha-ap492:36.4_0": "pulinuppādakopi ca.",
  "tha-ap492:37.1_0": "Taraṇo dhammaruciko,",
  "tha-ap492:37.2_0": "sālamaṇḍapiyo tathā;",
  "tha-ap492:37.3_0": "Satāni dve honti gāthā,",
  "tha-ap492:37.4_0": "ūnavīsatimeva ca."
}