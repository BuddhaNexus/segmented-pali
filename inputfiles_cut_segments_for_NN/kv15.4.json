{
  "kv15.4:0.1_0": "Kathāvatthu",
  "kv15.4:0.2_0": "Mahāpaṇṇāsaka",
  "kv15.4:0.3_0": "Pannarasamavagga",
  "kv15.4:0.4_0": "Khaṇalayamuhuttakathā",
  "kv15.4:1.1_0": "Khaṇo parinipphanno, layo parinipphanno, muhuttaṁ parinipphannanti?",
  "kv15.4:1.2_0": "Āmantā.",
  "kv15.4:1.3_0": "Rūpanti?",
  "kv15.4:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv15.4:1.5_0": "vedanā …",
  "kv15.4:1.6_0": "saññā …",
  "kv15.4:1.7_0": "saṅkhārā …",
  "kv15.4:1.8_0": "viññāṇanti?",
  "kv15.4:1.9_0": "Na hevaṁ vattabbe …pe….",
  "kv15.4:2.1_0": "Na vattabbaṁ—",
  "kv15.4:2.2_0": "“muhuttaṁ parinipphannan”ti?",
  "kv15.4:2.3_0": "Āmantā.",
  "kv15.4:2.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv15.4:2.5_0": "“tīṇimāni, bhikkhave, kathāvatthūni.",
  "kv15.4:2.6_0": "Katamāni tīṇi?",
  "kv15.4:2.7_0": "Atītaṁ vā, bhikkhave,",
  "kv15.4:2.7_1": "addhānaṁ ārabbha kathaṁ katheyya—",
  "kv15.4:2.8_0": "‘evaṁ ahosi atītamaddhānan’ti;",
  "kv15.4:2.9_0": "anāgataṁ vā, bhikkhave,",
  "kv15.4:2.9_1": "addhānaṁ ārabbha kathaṁ katheyya—",
  "kv15.4:2.10_0": "‘evaṁ bhavissati anāgatamaddhānan’ti;",
  "kv15.4:2.11_0": "etarahi vā, bhikkhave,",
  "kv15.4:2.11_1": "paccuppannaṁ addhānaṁ ārabbha kathaṁ katheyya—",
  "kv15.4:2.12_0": "‘evaṁ hoti etarahi paccuppannan’ti.",
  "kv15.4:2.13_0": "Imāni kho, bhikkhave, tīṇi kathāvatthūnī”ti.",
  "kv15.4:2.14_0": "Attheva suttantoti?",
  "kv15.4:2.15_0": "Āmantā.",
  "kv15.4:2.16_0": "Tena hi muhuttaṁ parinipphannanti.",
  "kv15.4:2.17_0": "Khaṇalayamuhuttakathā niṭṭhitā."
}