{
  "bv19:0.1_0": "Buddhavaṁsa",
  "bv19:0.2_0": "19. Tissabuddhavaṁsa",
  "bv19:1.1_0": "“Siddhatthassa aparena,",
  "bv19:1.2_0": "Asamo appaṭipuggalo;",
  "bv19:1.3_0": "Anantatejo amitayaso,",
  "bv19:1.4_0": "Tisso lokagganāyako.",
  "bv19:2.1_0": "Tamandhakāraṁ vidhamitvā,",
  "bv19:2.2_0": "obhāsetvā sadevakaṁ;",
  "bv19:2.3_0": "Anukampako mahāvīro,",
  "bv19:2.4_0": "loke uppajji cakkhumā.",
  "bv19:3.1_0": "Tassāpi atulā iddhi,",
  "bv19:3.2_0": "atulaṁ sīlaṁ samādhi ca;",
  "bv19:3.3_0": "Sabbattha pāramiṁ gantvā,",
  "bv19:3.4_0": "dhammacakkaṁ pavattayi.",
  "bv19:4.1_0": "So buddho dasasahassimhi,",
  "bv19:4.2_0": "Viññāpesi giraṁ suciṁ;",
  "bv19:4.3_0": "Koṭisatāni abhisamiṁsu,",
  "bv19:4.4_0": "Paṭhame dhammadesane.",
  "bv19:5.1_0": "Dutiyo navutikoṭīnaṁ,",
  "bv19:5.2_0": "tatiyo saṭṭhikoṭiyo;",
  "bv19:5.3_0": "Bandhanāto pamocesi,",
  "bv19:5.4_0": "satte naramarū tadā.",
  "bv19:6.1_0": "Sannipātā tayo āsuṁ,",
  "bv19:6.2_0": "tisse lokagganāyake;",
  "bv19:6.3_0": "Khīṇāsavānaṁ vimalānaṁ,",
  "bv19:6.4_0": "santacittāna tādinaṁ.",
  "bv19:7.1_0": "Khīṇāsavasatasahassānaṁ,",
  "bv19:7.2_0": "Paṭhamo āsi samāgamo;",
  "bv19:7.3_0": "Navutisatasahassānaṁ,",
  "bv19:7.4_0": "Dutiyo āsi samāgamo.",
  "bv19:8.1_0": "Asītisatasahassānaṁ,",
  "bv19:8.2_0": "tatiyo āsi samāgamo;",
  "bv19:8.3_0": "Khīṇāsavānaṁ vimalānaṁ,",
  "bv19:8.4_0": "pupphitānaṁ vimuttiyā.",
  "bv19:9.1_0": "Ahaṁ tena samayena,",
  "bv19:9.2_0": "sujāto nāma khattiyo;",
  "bv19:9.3_0": "Mahābhogaṁ chaḍḍayitvā,",
  "bv19:9.4_0": "pabbajiṁ isipabbajaṁ.",
  "bv19:10.1_0": "Mayi pabbajite sante,",
  "bv19:10.2_0": "uppajji lokanāyako;",
  "bv19:10.3_0": "Buddhoti saddaṁ sutvāna,",
  "bv19:10.4_0": "pīti me upapajjatha.",
  "bv19:11.1_0": "Dibbaṁ mandāravaṁ pupphaṁ,",
  "bv19:11.2_0": "padumaṁ pārichattakaṁ;",
  "bv19:11.3_0": "Ubho hatthehi paggayha,",
  "bv19:11.4_0": "dhunamāno upāgamiṁ.",
  "bv19:12.1_0": "Catuvaṇṇaparivutaṁ,",
  "bv19:12.2_0": "tissaṁ lokagganāyakaṁ;",
  "bv19:12.3_0": "Tamahaṁ pupphaṁ gahetvā,",
  "bv19:12.4_0": "matthake dhārayiṁ jinaṁ.",
  "bv19:13.1_0": "Sopi maṁ buddho byākāsi,",
  "bv19:13.2_0": "janamajjhe nisīdiya;",
  "bv19:13.3_0": "‘Dvenavute ito kappe,",
  "bv19:13.4_0": "ayaṁ buddho bhavissati.",
  "bv19:14.1_0": "Padhānaṁ padahitvāna,",
  "bv19:14.2_0": "…pe…",
  "bv19:14.3_0": "hessāma sammukhā imaṁ’.",
  "bv19:15.1_0": "Tassāpi vacanaṁ sutvā,",
  "bv19:15.2_0": "bhiyyo cittaṁ pasādayiṁ;",
  "bv19:15.3_0": "Uttariṁ vatamadhiṭṭhāsiṁ,",
  "bv19:15.4_0": "dasapāramipūriyā.",
  "bv19:16.1_0": "Khemakaṁ nāma nagaraṁ,",
  "bv19:16.2_0": "janasandho nāma khattiyo;",
  "bv19:16.3_0": "Padumā nāma janikā,",
  "bv19:16.4_0": "tissassa ca mahesino.",
  "bv19:17.1_0": "Sattavassasahassāni,",
  "bv19:17.2_0": "Agāraṁ ajjha so vasi;",
  "bv19:17.3_0": "Guhāsela nārisaya nisabhā,",
  "bv19:17.4_0": "Tayo pāsādamuttamā.",
  "bv19:18.1_0": "Samatiṁsasahassāni,",
  "bv19:18.2_0": "nāriyo samalaṅkatā;",
  "bv19:18.3_0": "Subhaddānāmikā nārī,",
  "bv19:18.4_0": "ānando nāma atrajo.",
  "bv19:19.1_0": "Nimitte caturo disvā,",
  "bv19:19.2_0": "assayānena nikkhami;",
  "bv19:19.3_0": "Anūnaaṭṭhamāsāni,",
  "bv19:19.4_0": "padhānaṁ padahī jino.",
  "bv19:20.1_0": "Brahmunā yācito santo,",
  "bv19:20.2_0": "tisso lokagganāyako;",
  "bv19:20.3_0": "Vatti cakkaṁ mahāvīro,",
  "bv19:20.4_0": "yasavatiyamuttame.",
  "bv19:21.1_0": "Brahmadevo udayo ca,",
  "bv19:21.2_0": "ahesuṁ aggasāvakā;",
  "bv19:21.3_0": "Samaṅgo nāmupaṭṭhāko,",
  "bv19:21.4_0": "tissassa ca mahesino.",
  "bv19:22.1_0": "Phussā ceva sudattā ca,",
  "bv19:22.2_0": "ahesuṁ aggasāvikā;",
  "bv19:22.3_0": "Bodhi tassa bhagavato,",
  "bv19:22.4_0": "asanoti pavuccati.",
  "bv19:23.1_0": "Sambalo ca sirimā ceva,",
  "bv19:23.2_0": "ahesuṁ aggupaṭṭhakā;",
  "bv19:23.3_0": "Kisāgotamī upasenā,",
  "bv19:23.4_0": "ahesuṁ aggupaṭṭhikā.",
  "bv19:24.1_0": "So buddho saṭṭhiratano,",
  "bv19:24.2_0": "ahu uccattane jino;",
  "bv19:24.3_0": "Anūpamo asadiso,",
  "bv19:24.4_0": "himavā viya dissati.",
  "bv19:25.1_0": "Tassāpi atulatejassa,",
  "bv19:25.2_0": "āyu āsi anuttaro;",
  "bv19:25.3_0": "Vassasatasahassāni,",
  "bv19:25.4_0": "loke aṭṭhāsi cakkhumā.",
  "bv19:26.1_0": "Uttamaṁ pavaraṁ seṭṭhaṁ,",
  "bv19:26.2_0": "anubhotvā mahāyasaṁ;",
  "bv19:26.3_0": "Jalitvā aggikkhandhova,",
  "bv19:26.4_0": "nibbuto so sasāvako.",
  "bv19:27.1_0": "Valāhakova anilena,",
  "bv19:27.2_0": "sūriyena viya ussavo;",
  "bv19:27.3_0": "Andhakārova padīpena,",
  "bv19:27.4_0": "nibbuto so sasāvako.",
  "bv19:28.1_0": "Tisso jinavaro buddho,",
  "bv19:28.2_0": "Nandārāmamhi nibbuto;",
  "bv19:28.3_0": "Tatthevassa jinathūpo,",
  "bv19:28.4_0": "Tīṇiyojanamuggato”ti. ",
  "bv19:29.1_0": "Tissassa bhagavato vaṁso sattarasamo."
}