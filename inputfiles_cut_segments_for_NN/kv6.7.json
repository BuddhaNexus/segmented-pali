{
  "kv6.7:0.1_0": "Kathāvatthu",
  "kv6.7:0.2_0": "Mahāpaṇṇāsaka",
  "kv6.7:0.3_0": "Chaṭṭhavagga",
  "kv6.7:0.4_0": "Ākāsosanidassanotikathā",
  "kv6.7:1.1_0": "Ākāso sanidassanoti?",
  "kv6.7:1.2_0": "Āmantā.",
  "kv6.7:1.3_0": "Rūpaṁ rūpāyatanaṁ rūpadhātu nīlaṁ pītakaṁ lohitakaṁ odātaṁ cakkhuviññeyyaṁ cakkhusmiṁ paṭihaññati cakkhussa āpāthaṁ āgacchatīti?",
  "kv6.7:1.4_0": "Na hevaṁ vattabbe …pe….",
  "kv6.7:2.1_0": "Ākāso sanidassanoti?",
  "kv6.7:2.2_0": "Āmantā.",
  "kv6.7:2.3_0": "Cakkhuñca paṭicca ākāsañca uppajjati cakkhuviññāṇanti?",
  "kv6.7:2.4_0": "Na hevaṁ vattabbe …pe….",
  "kv6.7:3.1_0": "Cakkhuñca paṭicca ākāsañca uppajjati cakkhuviññāṇanti?",
  "kv6.7:3.2_0": "Āmantā.",
  "kv6.7:3.3_0": "“Cakkhuñca paṭicca ākāsañca uppajjati cakkhuviññāṇan”ti—",
  "kv6.7:3.4_0": "attheva suttantoti?",
  "kv6.7:3.5_0": "Natthi.",
  "kv6.7:3.6_0": "“Cakkhuñca paṭicca rūpe ca uppajjati cakkhuviññāṇan”ti—",
  "kv6.7:3.7_0": "attheva suttantoti?",
  "kv6.7:3.8_0": "Āmantā.",
  "kv6.7:3.9_0": "Hañci “cakkhuñca paṭicca rūpe ca uppajjati cakkhuviññāṇan”ti—",
  "kv6.7:3.10_0": "attheva suttanto, no ca vata re vattabbe—",
  "kv6.7:3.11_0": "“cakkhuñca paṭicca ākāsañca uppajjati cakkhuviññāṇan”ti.",
  "kv6.7:4.1_0": "Na vattabbaṁ—",
  "kv6.7:4.2_0": "“ākāso sanidassano”ti?",
  "kv6.7:4.3_0": "Āmantā.",
  "kv6.7:4.4_0": "Nanu passati dvinnaṁ rukkhānaṁ antaraṁ, dvinnaṁ thambhānaṁ antaraṁ,",
  "kv6.7:4.4_1": "tāḷacchiddaṁ vātapānacchiddanti?",
  "kv6.7:4.5_0": "Āmantā.",
  "kv6.7:4.6_0": "Hañci passati dvinnaṁ rukkhānaṁ antaraṁ, dvinnaṁ thambhānaṁ antaraṁ,",
  "kv6.7:4.6_1": "tāḷacchiddaṁ vātapānacchiddaṁ, tena vata re vattabbe—",
  "kv6.7:4.7_0": "“ākāso sanidassano”ti.",
  "kv6.7:4.8_0": "Ākāso sanidassanotikathā niṭṭhitā."
}