{
  "tha-ap360:0.1_0": "Therāpadāna",
  "tha-ap360:0.2_0": "Saddasaññakavagga",
  "tha-ap360:0.3_0": "8 Mañcadāyakattheraapadāna",
  "tha-ap360:1.1_0": "“Vipassino bhagavato,",
  "tha-ap360:1.2_0": "lokajeṭṭhassa tādino;",
  "tha-ap360:1.3_0": "Ekaṁ mañcaṁ mayā dinnaṁ,",
  "tha-ap360:1.4_0": "pasannena sapāṇinā.",
  "tha-ap360:2.1_0": "Hatthiyānaṁ assayānaṁ,",
  "tha-ap360:2.2_0": "dibbayānaṁ samajjhagaṁ;",
  "tha-ap360:2.3_0": "Tena mañcakadānena,",
  "tha-ap360:2.4_0": "pattomhi āsavakkhayaṁ.",
  "tha-ap360:3.1_0": "Ekanavutito kappe,",
  "tha-ap360:3.2_0": "yaṁ mañcamadadiṁ tadā;",
  "tha-ap360:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap360:3.4_0": "mañcadānassidaṁ phalaṁ.",
  "tha-ap360:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap360:4.2_0": "…pe…",
  "tha-ap360:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap360:5.1_0": "Itthaṁ sudaṁ āyasmā mañcadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap360:6.1_0": "Mañcadāyakattherassāpadānaṁ aṭṭhamaṁ."
}