{
  "vv19:0.1_0": "Vimānavatthu",
  "vv19:0.2_0": "Itthivimāna",
  "vv19:0.3_0": "Cittalatāvagga",
  "vv19:0.4_0": "2. Lakhumāvimānavatthu",
  "vv19:1.1_0": "“Abhikkantena vaṇṇena,",
  "vv19:1.2_0": "yā tvaṁ tiṭṭhasi devate;",
  "vv19:1.3_0": "Obhāsentī disā sabbā,",
  "vv19:1.4_0": "osadhī viya tārakā.",
  "vv19:2.1_0": "Kena tetādiso vaṇṇo,",
  "vv19:2.2_0": "kena te idha mijjhati;",
  "vv19:2.3_0": "Uppajjanti ca te bhogā,",
  "vv19:2.4_0": "ye keci manaso piyā.",
  "vv19:3.1_0": "Pucchāmi taṁ devi mahānubhāve,",
  "vv19:3.2_0": "Manussabhūtā kimakāsi puññaṁ;",
  "vv19:3.3_0": "Kenāsi evaṁ jalitānubhāvā,",
  "vv19:3.4_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv19:4.1_0": "Sā devatā attamanā,",
  "vv19:4.2_0": "moggallānena pucchitā;",
  "vv19:4.3_0": "Pañhaṁ puṭṭhā viyākāsi,",
  "vv19:4.4_0": "yassa kammassidaṁ phalaṁ.",
  "vv19:5.1_0": "“Kevaṭṭadvārā nikkhamma,",
  "vv19:5.2_0": "ahu mayhaṁ nivesanaṁ;",
  "vv19:5.3_0": "Tattha sañcaramānānaṁ,",
  "vv19:5.4_0": "sāvakānaṁ mahesinaṁ.",
  "vv19:6.1_0": "Odanaṁ kummāsaṁ ḍākaṁ,",
  "vv19:6.2_0": "loṇasovīrakañcahaṁ;",
  "vv19:6.3_0": "Adāsiṁ ujubhūtesu,",
  "vv19:6.4_0": "vippasannena cetasā.",
  "vv19:7.1_0": "Cātuddasiṁ pañcadasiṁ,",
  "vv19:7.2_0": "yā ca pakkhassa aṭṭhamī;",
  "vv19:7.3_0": "Pāṭihāriyapakkhañca,",
  "vv19:7.4_0": "aṭṭhaṅgasusamāgataṁ.",
  "vv19:8.1_0": "Uposathaṁ upavasissaṁ,",
  "vv19:8.2_0": "sadā sīlesu saṁvutā;",
  "vv19:8.3_0": "Saññamā saṁvibhāgā ca,",
  "vv19:8.4_0": "vimānaṁ āvasāmahaṁ.",
  "vv19:9.1_0": "Pāṇātipātā viratā,",
  "vv19:9.2_0": "musāvādā ca saññatā;",
  "vv19:9.3_0": "Theyyā ca aticārā ca,",
  "vv19:9.4_0": "majjapānā ca ārakā.",
  "vv19:10.1_0": "Pañcasikkhāpade ratā,",
  "vv19:10.2_0": "ariyasaccāna kovidā;",
  "vv19:10.3_0": "Upāsikā cakkhumato,",
  "vv19:10.4_0": "gotamassa yasassino.",
  "vv19:11.1_0": "Tena metādiso vaṇṇo,",
  "vv19:11.2_0": "…pe…",
  "vv19:11.3_0": "Vaṇṇo ca me sabbadisā pabhāsatīti.",
  "vv19:12.1_0": "Mama ca, bhante, vacanena bhagavato pāde sirasā vandeyyāsi—",
  "vv19:12.2_0": "‘lakhumā nāma, bhante, upāsikā bhagavato pāde sirasā vandatī’ti.",
  "vv19:12.3_0": "Anacchariyaṁ kho panetaṁ, bhante, yaṁ maṁ bhagavā aññatarasmiṁ sāmaññaphale byākareyya.",
  "vv19:12.4_0": "Taṁ bhagavā sakadāgāmiphale byākāsī”ti.",
  "vv19:13.1_0": "Lakhumāvimānaṁ dutiyaṁ."
}