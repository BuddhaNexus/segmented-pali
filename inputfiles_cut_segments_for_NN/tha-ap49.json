{
  "tha-ap49:0.1_0": "Therāpadāna",
  "tha-ap49:0.2_0": "Upālivagga",
  "tha-ap49:0.3_0": "7. Sayanadāyakattheraapadāna",
  "tha-ap49:1.1_0": "“Siddhatthassa bhagavato,",
  "tha-ap49:1.2_0": "mettacittassa tādino;",
  "tha-ap49:1.3_0": "Sayanaggaṁ mayā dinnaṁ,",
  "tha-ap49:1.4_0": "dussabhaṇḍehi atthataṁ.",
  "tha-ap49:2.1_0": "Paṭiggahesi bhagavā,",
  "tha-ap49:2.2_0": "kappiyaṁ sayanāsanaṁ;",
  "tha-ap49:2.3_0": "Uṭṭhāya sayanā tamhā,",
  "tha-ap49:2.4_0": "vehāsaṁ uggamī jino.",
  "tha-ap49:3.1_0": "Catunnavutito kappe,",
  "tha-ap49:3.2_0": "yaṁ sayanamadāsahaṁ;",
  "tha-ap49:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap49:3.4_0": "sayanassa idaṁ phalaṁ.",
  "tha-ap49:4.1_0": "Ekapaññāsito kappe,",
  "tha-ap49:4.2_0": "varako devasavhayo;",
  "tha-ap49:4.3_0": "Sattaratanasampanno,",
  "tha-ap49:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap49:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap49:5.2_0": "…pe…",
  "tha-ap49:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap49:6.1_0": "Itthaṁ sudaṁ āyasmā sayanadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap49:7.1_0": "Sayanadāyakattherassāpadānaṁ sattamaṁ."
}