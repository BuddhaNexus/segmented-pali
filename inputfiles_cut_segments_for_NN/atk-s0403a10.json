{
  "atk-s0403a10:0_0": "(11) 1. Phāsuvihāravaggo",
  "atk-s0403a10:1_0": "1. Sārajjasuttavaṇṇanā",
  "atk-s0403a10:2_0": "101. Tatiyassa paṭhame vesārajjakaraṇāti visāradabhāvāvahā. Sārajjaṁ hotīti domanassaṁ hoti.",
  "atk-s0403a10:3_0": "2. Ussaṅkitasuttavaṇṇanā",
  "atk-s0403a10:4_0": "102.",
  "atk-s0403a10:4_1": "Dutiye ussaṅkitaparisaṅkitoti ussaṅkito ca parisaṅkito ca.",
  "atk-s0403a10:4_2": "Api akuppadhammopīti api akuppadhammo khīṇāsavo samānopi parehi pāpabhikkhūhi ussaṅkitaparisaṅkito hotīti attho.",
  "atk-s0403a10:4_3": "Vesiyāgocarotiādīsu vesiyā vuccanti rūpūpajīviniyo,",
  "atk-s0403a10:4_4": "tā gocaro assāti vesiyāgocaro,",
  "atk-s0403a10:4_5": "tāsaṁ gehaṁ abhiṇhagamanoti attho.",
  "atk-s0403a10:4_6": "Sesapadesupi eseva nayo.",
  "atk-s0403a10:4_7": "Tattha pana vidhavāti matapatikā.",
  "atk-s0403a10:4_8": "Thullakumārikāti mahallikakumārikāyo.",
  "atk-s0403a10:5_0": "3. Mahācorasuttavaṇṇanā",
  "atk-s0403a10:6_0": "103.",
  "atk-s0403a10:6_1": "Tatiye ito bhogena paṭisantharissāmīti ito mama sāpateyyato bhogaṁ gahetvā tena paṭisanthāraṁ karissāmi,",
  "atk-s0403a10:6_2": "tassa ca mama ca antaraṁ pidahissāmīti attho.",
  "atk-s0403a10:6_3": "Gahaṇānīti parasantakānaṁ bhaṇḍānaṁ gahaṇāni.",
  "atk-s0403a10:6_4": "Guyhamantāti guhitabbamantā.",
  "atk-s0403a10:6_5": "Antaggāhikāyāti sassataṁ vā ucchedaṁ vā gahetvā ṭhitāya.",
  "atk-s0403a10:6_6": "Sesamettha uttānatthameva.",
  "atk-s0403a10:6_7": "Catutthe sabbaṁ heṭṭhā vuttanayameva.",
  "atk-s0403a10:7_0": "5. Phāsuvihārasuttavaṇṇanā",
  "atk-s0403a10:8_0": "105.",
  "atk-s0403a10:8_1": "Pañcame mettaṁ kāyakammanti mettacittena pavattitaṁ kāyakammaṁ.",
  "atk-s0403a10:8_2": "Āvi ceva raho cāti sammukhe ceva parammukhe ca.",
  "atk-s0403a10:8_3": "Itaresupi eseva nayo.",
  "atk-s0403a10:8_4": "Yāni tāni sīlānītiādi catupārisuddhisīlavasena vuttaṁ.",
  "atk-s0403a10:8_5": "Samādhisaṁvattanikānīti maggasamādhiphalasamādhinibbattakāni.",
  "atk-s0403a10:8_6": "Sīlasāmaññagatoti samānasīlataṁ gato,",
  "atk-s0403a10:8_7": "ekasadisasīlo hutvāti attho.",
  "atk-s0403a10:8_8": "Takkarassāti yo naṁ karoti, tassa.",
  "atk-s0403a10:8_9": "Iti imasmiṁ sutte sīlaṁ missakaṁ kathitaṁ,",
  "atk-s0403a10:8_10": "diṭṭhi vipassanāsammādiṭṭhīti.",
  "atk-s0403a10:9_0": "6. Ānandasuttavaṇṇanā",
  "atk-s0403a10:10_0": "106.",
  "atk-s0403a10:10_1": "Chaṭṭhe no ca paraṁ adhisīle sampavattā hotīti paraṁ sīlabhāvena na garahati na upavadati.",
  "atk-s0403a10:10_2": "Attānupekkhīti attanova katākataṁ jānanavasena attānaṁ anupekkhitā.",
  "atk-s0403a10:10_3": "No parānupekkhīti parassa katākatesu abyāvaṭo.",
  "atk-s0403a10:10_4": "Apaññātoti apākaṭo appapuñño.",
  "atk-s0403a10:10_5": "Apaññātakenāti apaññātabhāvena apākaṭatāya mandapuññatāya.",
  "atk-s0403a10:10_6": "No paritassatīti paritāsaṁ nāpajjati.",
  "atk-s0403a10:10_7": "Iti imasmiṁ sutte khīṇāsavova kathito.",
  "atk-s0403a10:11_0": "7-8. Sīlasuttādivaṇṇanā",
  "atk-s0403a10:12_0": "107-108.",
  "atk-s0403a10:12_1": "Sattame sīlasamādhipaññā missikā kathitā,",
  "atk-s0403a10:12_2": "vimutti arahattaphalaṁ ,",
  "atk-s0403a10:12_3": "vimuttiñāṇadassanaṁ paccavekkhaṇañāṇaṁ lokiyameva.",
  "atk-s0403a10:12_4": "Aṭṭhamepi eseva nayo.",
  "atk-s0403a10:12_5": "Paccavekkhaṇañāṇaṁ panettha asekhassa pavattattā asekhanti vuttaṁ.",
  "atk-s0403a10:13_0": "9-10. Cātuddisasuttādivaṇṇanā",
  "atk-s0403a10:14_0": "109-110.",
  "atk-s0403a10:14_1": "Navame cātuddisoti catūsu disāsu appaṭihatacāro.",
  "atk-s0403a10:14_2": "Imasmimpi sutte khīṇāsavova kathito.",
  "atk-s0403a10:14_3": "Dasame alanti yutto.",
  "atk-s0403a10:14_4": "Idhāpi khīṇāsavova kathito.",
  "atk-s0403a10:15_0": "Phāsuvihāravaggo paṭhamo."
}