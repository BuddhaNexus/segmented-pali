{
  "mil3.2.1:0.1_0": "Milindapañha",
  "mil3.2.1:0.2_0": "Addhānavagga",
  "mil3.2.1:0.3_0": "1. Dhammasantatipañha",
  "mil3.2.1:1.1_0": "Rājā āha—",
  "mil3.2.1:1.2_0": "“bhante nāgasena, yo uppajjati, so eva so, udāhu añño”ti?",
  "mil3.2.1:1.3_0": "Thero āha—",
  "mil3.2.1:1.4_0": "“na ca so, na ca añño”ti.",
  "mil3.2.1:2.1_0": "“Opammaṁ karohī”ti.",
  "mil3.2.1:2.2_0": "“Taṁ kiṁ maññasi, mahārāja,",
  "mil3.2.1:2.2_1": "yadā tvaṁ daharo taruṇo mando uttānaseyyako ahosi,",
  "mil3.2.1:2.2_2": "soyeva tvaṁ etarahi mahanto”ti?",
  "mil3.2.1:2.3_0": "“Na hi, bhante, añño so daharo taruṇo mando uttānaseyyako ahosi, añño ahaṁ etarahi mahanto”ti.",
  "mil3.2.1:2.4_0": "“Evaṁ sante kho, mahārāja,",
  "mil3.2.1:2.4_1": "mātātipi na bhavissati,",
  "mil3.2.1:2.4_2": "pitātipi na bhavissati,",
  "mil3.2.1:2.4_3": "ācariyotipi na bhavissati,",
  "mil3.2.1:2.4_4": "sippavātipi na bhavissati,",
  "mil3.2.1:2.4_5": "sīlavātipi na bhavissati,",
  "mil3.2.1:2.4_6": "paññavātipi na bhavissati.",
  "mil3.2.1:2.5_0": "Kiṁ nu kho, mahārāja,",
  "mil3.2.1:2.5_1": "aññā eva kalalassa mātā,",
  "mil3.2.1:2.5_2": "aññā abbudassa mātā,",
  "mil3.2.1:2.5_3": "aññā pesiyā mātā,",
  "mil3.2.1:2.5_4": "aññā ghanassa mātā,",
  "mil3.2.1:2.5_5": "aññā khuddakassa mātā,",
  "mil3.2.1:2.5_6": "aññā mahantassa mātā,",
  "mil3.2.1:2.5_7": "añño sippaṁ sikkhati,",
  "mil3.2.1:2.5_8": "añño sikkhito bhavati,",
  "mil3.2.1:2.5_9": "añño pāpakammaṁ karoti,",
  "mil3.2.1:2.5_10": "aññassa hatthapādā chijjantī”ti?",
  "mil3.2.1:2.6_0": "“Na hi, bhante.",
  "mil3.2.1:2.7_0": "Tvaṁ pana, bhante, evaṁ vutte, kiṁ vadeyyāsī”ti?",
  "mil3.2.1:2.8_0": "Thero āha—",
  "mil3.2.1:2.9_0": "“ahaññeva kho, mahārāja,",
  "mil3.2.1:2.9_1": "daharo ahosiṁ taruṇo mando uttānaseyyako,",
  "mil3.2.1:2.9_2": "ahaññeva etarahi mahanto,",
  "mil3.2.1:2.9_3": "imameva kāyaṁ nissāya sabbe te ekasaṅgahitā”ti.",
  "mil3.2.1:3.1_0": "“Bhiyyo opammaṁ karohī”ti.",
  "mil3.2.1:3.2_0": "“Yathā, mahārāja, kocideva puriso padīpaṁ padīpeyya, kiṁ so sabbarattiṁ padīpeyyā”ti?",
  "mil3.2.1:3.3_0": "“Āma, bhante, sabbarattiṁ padīpeyyā”ti.",
  "mil3.2.1:3.4_0": "“Kiṁ nu kho, mahārāja, yā purime yāme acci, sā majjhime yāme accī”ti?",
  "mil3.2.1:3.5_0": "“Na hi, bhante”ti.",
  "mil3.2.1:3.6_0": "“Yā majjhime yāme acci, sā pacchime yāme accī”ti?",
  "mil3.2.1:3.7_0": "“Na hi, bhante”ti.",
  "mil3.2.1:3.8_0": "“Kiṁ nu kho, mahārāja,",
  "mil3.2.1:3.8_1": "añño so ahosi purime yāme padīpo,",
  "mil3.2.1:3.8_2": "añño majjhime yāme padīpo,",
  "mil3.2.1:3.8_3": "añño pacchime yāme padīpo”ti?",
  "mil3.2.1:3.9_0": "“Na hi, bhante, taṁyeva nissāya sabbarattiṁ padīpito”ti.",
  "mil3.2.1:3.10_0": "“Evameva kho, mahārāja,",
  "mil3.2.1:3.10_1": "dhammasantati sandahati,",
  "mil3.2.1:3.10_2": "añño uppajjati,",
  "mil3.2.1:3.10_3": "añño nirujjhati,",
  "mil3.2.1:3.10_4": "apubbaṁ acarimaṁ viya sandahati,",
  "mil3.2.1:3.10_5": "tena na ca so,",
  "mil3.2.1:3.10_6": "na ca añño,",
  "mil3.2.1:3.10_7": "purimaviññāṇe pacchimaviññāṇaṁ saṅgahaṁ gacchatī”ti.",
  "mil3.2.1:4.1_0": "“Bhiyyo opammaṁ karohī”ti.",
  "mil3.2.1:4.2_0": "“Yathā, mahārāja,",
  "mil3.2.1:4.2_1": "khīraṁ duyhamānaṁ kālantarena dadhi parivatteyya,",
  "mil3.2.1:4.2_2": "dadhito navanītaṁ,",
  "mil3.2.1:4.2_3": "navanītato ghataṁ parivatteyya,",
  "mil3.2.1:4.2_4": "yo nu kho, mahārāja,",
  "mil3.2.1:4.2_5": "evaṁ vadeyya ‘yaṁyeva khīraṁ taṁyeva dadhi,",
  "mil3.2.1:4.2_6": "yaṁyeva dadhi taṁyeva navanītaṁ,",
  "mil3.2.1:4.2_7": "yaṁyeva navanītaṁ taṁyeva ghatan’ti,",
  "mil3.2.1:4.2_8": "sammā nu kho so, mahārāja,",
  "mil3.2.1:4.2_9": "vadamāno vadeyyā”ti?",
  "mil3.2.1:4.3_0": "“Na hi, bhante, taṁyeva nissāya sambhūtan”ti.",
  "mil3.2.1:4.4_0": "“Evameva kho, mahārāja,",
  "mil3.2.1:4.4_1": "dhammasantati sandahati,",
  "mil3.2.1:4.4_2": "añño uppajjati,",
  "mil3.2.1:4.4_3": "añño nirujjhati,",
  "mil3.2.1:4.4_4": "apubbaṁ acarimaṁ viya sandahati,",
  "mil3.2.1:4.4_5": "tena na ca so,",
  "mil3.2.1:4.4_6": "na ca añño,",
  "mil3.2.1:4.4_7": "purimaviññāṇe pacchimaviññāṇaṁ saṅgahaṁ gacchatī”ti.",
  "mil3.2.1:5.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.2.1:6.1_0": "Dhammasantatipañho paṭhamo."
}