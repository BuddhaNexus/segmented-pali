{
  "ja40:0.1_0": "Jātaka",
  "ja40:0.2_0": "Ekakanipāta",
  "ja40:0.3_0": "Kulāvakavagga",
  "ja40:0.4_0": "10. Khadiraṅgārajātaka",
  "ja40:1.1_0": "“Kāmaṁ patāmi nirayaṁ,",
  "ja40:1.2_0": "uddhampādo avaṁsiro;",
  "ja40:1.3_0": "Nānariyaṁ karissāmi,",
  "ja40:1.4_0": "handa piṇḍaṁ paṭiggahā”ti.",
  "ja40:2.1_0": "Khadiraṅgārajātakaṁ dasamaṁ.",
  "ja40:2.2_0": "Kulāvakavaggo catuttho.",
  "ja40:3.1_0": "Tassuddānaṁ",
  "ja40:4.1_0": "Sirimātali dhītara pakkhivaro,",
  "ja40:4.2_0": "Ratiyāgato mātāpitā ca puna;",
  "ja40:4.3_0": "Jagatīruha vuḍḍha sukakkaṭako,",
  "ja40:4.4_0": "Tathā nandakapiṇḍavarena dasāti."
}