{
  "mil3.3.14:0.1_0": "Milindapañha",
  "mil3.3.14:0.2_0": "Vicāravagga",
  "mil3.3.14:0.3_0": "14. Vicāralakkhaṇapañha",
  "mil3.3.14:1.1_0": "“Bhante nāgasena, kiṁlakkhaṇo vicāro”ti?",
  "mil3.3.14:1.2_0": "“Anumajjanalakkhaṇo, mahārāja, vicāro”ti.",
  "mil3.3.14:2.1_0": "“Opammaṁ karohī”ti.",
  "mil3.3.14:2.2_0": "“Yathā, mahārāja,",
  "mil3.3.14:2.2_1": "kaṁsathālaṁ ākoṭitaṁ pacchā anuravati anusandahati, yathā, mahārāja, ākoṭanā,",
  "mil3.3.14:2.2_2": "evaṁ vitakko daṭṭhabbo.",
  "mil3.3.14:2.3_0": "Yathā anuravanā, evaṁ vicāro daṭṭhabbo”ti.",
  "mil3.3.14:3.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.3.14:4.1_0": "Vicāralakkhaṇapañho cuddasamo.",
  "mil3.3.14:5.1_0": "Vicāravaggo tatiyo.",
  "mil3.3.14:6.1_0": "Imasmiṁ vagge cuddasa pañhā."
}