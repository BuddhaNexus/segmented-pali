{
  "ja183:0.1_0": "Jātaka",
  "ja183:0.2_0": "Dukanipāta",
  "ja183:0.3_0": "Asadisavagga",
  "ja183:0.4_0": "3. Vālodakajātaka",
  "ja183:1.1_0": "“Vālodakaṁ apparasaṁ nihīnaṁ,",
  "ja183:1.2_0": "Pitvā mado jāyati gadrabhānaṁ;",
  "ja183:1.3_0": "Imañca pitvāna rasaṁ paṇītaṁ,",
  "ja183:1.4_0": "Mado na sañjāyati sindhavānaṁ”.",
  "ja183:2.1_0": "“Appaṁ pivitvāna nihīnajacco,",
  "ja183:2.2_0": "So majjatī tena janinda puṭṭho;",
  "ja183:2.3_0": "Dhorayhasīlī ca kulamhi jāto,",
  "ja183:2.4_0": "Na majjatī aggarasaṁ pivitvā”ti.",
  "ja183:3.1_0": "Vālodakajātakaṁ tatiyaṁ."
}