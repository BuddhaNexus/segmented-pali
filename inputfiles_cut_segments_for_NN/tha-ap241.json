{
  "tha-ap241:0.1_0": "Therāpadāna",
  "tha-ap241:0.2_0": "Udakāsanavagga",
  "tha-ap241:0.3_0": "9. Jagatikārakattheraapadāna",
  "tha-ap241:1.1_0": "“Nibbute lokanāthamhi,",
  "tha-ap241:1.2_0": "atthadassi naruttame;",
  "tha-ap241:1.3_0": "Jagatī kāritā mayhaṁ,",
  "tha-ap241:1.4_0": "buddhassa thūpamuttame.",
  "tha-ap241:2.1_0": "Aṭṭhārase kappasate,",
  "tha-ap241:2.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap241:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap241:2.4_0": "jagatiyā idaṁ phalaṁ.",
  "tha-ap241:3.1_0": "Paṭisambhidā catasso,",
  "tha-ap241:3.2_0": "…pe…",
  "tha-ap241:3.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap241:4.1_0": "Itthaṁ sudaṁ āyasmā jagatikārako thero imā gāthāyo abhāsitthāti.",
  "tha-ap241:5.1_0": "Jagatikārakattherassāpadānaṁ navamaṁ."
}