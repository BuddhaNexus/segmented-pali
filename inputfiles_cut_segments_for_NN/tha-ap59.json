{
  "tha-ap59:0.1_0": "Therāpadāna",
  "tha-ap59:0.2_0": "Bījanivagga",
  "tha-ap59:0.3_0": "7. Pañcadīpakattheraapadāna",
  "tha-ap59:1.1_0": "“Padumuttarabuddhassa,",
  "tha-ap59:1.2_0": "sabbabhūtānukampino;",
  "tha-ap59:1.3_0": "Saddahitvāna saddhamme,",
  "tha-ap59:1.4_0": "ujudiṭṭhi ahosahaṁ.",
  "tha-ap59:2.1_0": "Padīpadānaṁ pādāsiṁ,",
  "tha-ap59:2.2_0": "parivāretvāna bodhiyaṁ;",
  "tha-ap59:2.3_0": "Saddahanto padīpāni,",
  "tha-ap59:2.4_0": "akariṁ tāvade ahaṁ.",
  "tha-ap59:3.1_0": "Yaṁ yaṁ yonupapajjāmi,",
  "tha-ap59:3.2_0": "devattaṁ atha mānusaṁ;",
  "tha-ap59:3.3_0": "Ākāse ukkaṁ dhārenti,",
  "tha-ap59:3.4_0": "dīpadānassidaṁ phalaṁ.",
  "tha-ap59:4.1_0": "Tirokuṭṭaṁ tiroselaṁ,",
  "tha-ap59:4.2_0": "samatiggayha pabbataṁ;",
  "tha-ap59:4.3_0": "Samantā yojanasataṁ,",
  "tha-ap59:4.4_0": "dassanaṁ anubhomahaṁ.",
  "tha-ap59:5.1_0": "Tena kammāvasesena,",
  "tha-ap59:5.2_0": "pattomhi āsavakkhayaṁ;",
  "tha-ap59:5.3_0": "Dhāremi antimaṁ dehaṁ,",
  "tha-ap59:5.4_0": "dvipadindassa sāsane.",
  "tha-ap59:6.1_0": "Catuttiṁse kappasate,",
  "tha-ap59:6.2_0": "satacakkhusanāmakā;",
  "tha-ap59:6.3_0": "Rājāhesuṁ mahātejā,",
  "tha-ap59:6.4_0": "cakkavattī mahabbalā.",
  "tha-ap59:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap59:7.2_0": "…pe…",
  "tha-ap59:7.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap59:8.1_0": "Itthaṁ sudaṁ āyasmā pañcadīpako thero imā gāthāyo abhāsitthāti.",
  "tha-ap59:9.1_0": "Pañcadīpakattherassāpadānaṁ sattamaṁ."
}