{
  "tha-ap78:0.1_0": "Therāpadāna",
  "tha-ap78:0.2_0": "Nāgasamālavagga",
  "tha-ap78:0.3_0": "6 Tiṇasantharadāyakattheraapadāna",
  "tha-ap78:1.1_0": "“Himavantassāvidūre,",
  "tha-ap78:1.2_0": "mahājātassaro ahu;",
  "tha-ap78:1.3_0": "Satapattehi sañchanno,",
  "tha-ap78:1.4_0": "nānāsakuṇamālayo.",
  "tha-ap78:2.1_0": "Tamhi nhatvā ca pitvā ca,",
  "tha-ap78:2.2_0": "avidūre vasāmahaṁ;",
  "tha-ap78:2.3_0": "Addasaṁ samaṇānaggaṁ,",
  "tha-ap78:2.4_0": "gacchantaṁ anilañjase.",
  "tha-ap78:3.1_0": "Mama saṅkappamaññāya,",
  "tha-ap78:3.2_0": "satthā loke anuttaro;",
  "tha-ap78:3.3_0": "Abbhato oruhitvāna,",
  "tha-ap78:3.4_0": "bhūmiyaṁṭhāsi tāvade.",
  "tha-ap78:4.1_0": "Visāṇena tiṇaṁ gayha,",
  "tha-ap78:4.2_0": "nisīdanamadāsahaṁ;",
  "tha-ap78:4.3_0": "Nisīdi bhagavā tattha,",
  "tha-ap78:4.4_0": "tisso lokagganāyako.",
  "tha-ap78:5.1_0": "Sakaṁ cittaṁ pasādetvā,",
  "tha-ap78:5.2_0": "avandi lokanāyakaṁ;",
  "tha-ap78:5.3_0": "Paṭikuṭiko apasakkiṁ,",
  "tha-ap78:5.4_0": "nijjhāyanto mahāmuniṁ.",
  "tha-ap78:6.1_0": "Tena cittappasādena,",
  "tha-ap78:6.2_0": "nimmānaṁ upapajjahaṁ;",
  "tha-ap78:6.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap78:6.4_0": "santharassa idaṁ phalaṁ.",
  "tha-ap78:7.1_0": "Ito dutiyake kappe,",
  "tha-ap78:7.2_0": "miga sammatakhattiyo;",
  "tha-ap78:7.3_0": "Sattaratanasampanno,",
  "tha-ap78:7.4_0": "cakkavattī mahabbalo.",
  "tha-ap78:8.1_0": "Paṭisambhidā catasso,",
  "tha-ap78:8.2_0": "…pe…",
  "tha-ap78:8.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap78:9.1_0": "Itthaṁ sudaṁ āyasmā tiṇasantharadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap78:10.1_0": "Tiṇasantharadāyakattherassāpadānaṁ chaṭṭhaṁ."
}