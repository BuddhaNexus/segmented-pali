{
  "ne32:0.1_0": "Netti",
  "ne32:0.2_0": "Paṭiniddesavāra",
  "ne32:0.3_0": "Sampāta 13",
  "ne32:0.4_0": "Sodhanahārasampāta",
  "ne32:1.1_0": "Tattha katamo sodhano hārasampāto?",
  "ne32:2.1_0": "“Tasmā rakkhitacittassa, sammāsaṅkappagocaro”ti gāthā.",
  "ne32:2.2_0": "Yattha ārambho suddho, so pañho visajjito bhavati.",
  "ne32:2.3_0": "Yattha pana ārambho na suddho,",
  "ne32:2.3_1": "na tāva so pañho visajjito bhavati.",
  "ne32:2.4_0": "Niyutto sodhano hārasampāto."
}