{
  "ja389:0.1_0": "Jātaka",
  "ja389:0.2_0": "Chakkanipāta",
  "ja389:0.3_0": "Kharaputtavagga",
  "ja389:0.4_0": "4. Suvaṇṇakakkaṭajātaka",
  "ja389:1.1_0": "“Siṅgīmigo āyatacakkhunetto,",
  "ja389:1.2_0": "Aṭṭhittaco vārisayo alomo;",
  "ja389:1.3_0": "Tenābhibhūto kapaṇaṁ rudāmi,",
  "ja389:1.4_0": "Hare sakhā kissa nu maṁ jahāsi”.",
  "ja389:2.1_0": "So passasanto mahatā phaṇena,",
  "ja389:2.2_0": "Bhujaṅgamo kakkaṭamajjhapatto;",
  "ja389:2.3_0": "Sakhā sakhāraṁ paritāyamāno,",
  "ja389:2.4_0": "Bhujaṅgamaṁ kakkaṭako gahesi.",
  "ja389:3.1_0": "“Na vāyasaṁ no pana kaṇhasappaṁ,",
  "ja389:3.2_0": "Ghāsatthiko kakkaṭako adeyya;",
  "ja389:3.3_0": "Pucchāmi taṁ āyatacakkhunetta,",
  "ja389:3.4_0": "Atha kissa hetumha ubho gahītā”.",
  "ja389:4.1_0": "“Ayaṁ puriso mama atthakāmo,",
  "ja389:4.2_0": "Yo maṁ gahetvāna dakāya neti;",
  "ja389:4.3_0": "Tasmiṁ mate dukkhamanappakaṁ me,",
  "ja389:4.4_0": "Ahañca eso ca ubho na homa.",
  "ja389:5.1_0": "Mamañca disvāna pavaddhakāyaṁ,",
  "ja389:5.2_0": "Sabbo jano hiṁsitumeva micche;",
  "ja389:5.3_0": "Sāduñca thūlañca muduñca maṁsaṁ,",
  "ja389:5.4_0": "Kākāpi maṁ disvāna viheṭhayeyyuṁ”.",
  "ja389:6.1_0": "“Sacetassa hetumha ubho gahītā,",
  "ja389:6.2_0": "Uṭṭhātu poso visamāvamāmi;",
  "ja389:6.3_0": "Mamañca kākañca pamuñca khippaṁ,",
  "ja389:6.4_0": "Pure visaṁ gāḷhamupeti maccaṁ”.",
  "ja389:7.1_0": "“Sappaṁ pamokkhāmi na tāva kākaṁ,",
  "ja389:7.2_0": "Paṭibandhako hohiti tāva kāko;",
  "ja389:7.3_0": "Purisañca disvāna sukhiṁ arogaṁ,",
  "ja389:7.4_0": "Kākaṁ pamokkhāmi yatheva sappaṁ”.",
  "ja389:8.1_0": "“Kāko tadā devadatto ahosi,",
  "ja389:8.2_0": "Māro pana kaṇhasappo ahosi;",
  "ja389:8.3_0": "Ānandabhaddo kakkaṭako ahosi,",
  "ja389:8.4_0": "Ahaṁ tadā brāhmaṇo homi satthā”ti.",
  "ja389:9.1_0": "Suvaṇṇakakkaṭajātakaṁ catutthaṁ."
}