{
  "pli-tv-bi-vb-pc32:0.1_0": "Theravāda Vinaya",
  "pli-tv-bi-vb-pc32:0.2_0": "Bhikkhunivibhaṅga",
  "pli-tv-bi-vb-pc32:0.3_0": "Pācittiyakaṇḍa",
  "pli-tv-bi-vb-pc32:0.4_0": "Tuvaṭṭavagga",
  "pli-tv-bi-vb-pc32:0.5_0": "32. Ekattharaṇatuvaṭṭanasikkhāpada",
  "pli-tv-bi-vb-pc32:1.1_0": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bi-vb-pc32:1.2_0": "Tena kho pana samayena bhikkhuniyo dve ekattharaṇapāvuraṇā tuvaṭṭenti.",
  "pli-tv-bi-vb-pc32:1.3_0": "Manussā vihāracārikaṁ āhiṇḍantā passitvā ujjhāyanti khiyyanti vipācenti—",
  "pli-tv-bi-vb-pc32:1.4_0": "“kathañhi nāma bhikkhuniyo dve ekattharaṇapāvuraṇā tuvaṭṭessanti,",
  "pli-tv-bi-vb-pc32:1.4_1": "seyyathāpi gihiniyo kāmabhoginiyo”ti.",
  "pli-tv-bi-vb-pc32:1.5_0": "Assosuṁ kho bhikkhuniyo tesaṁ manussānaṁ ujjhāyantānaṁ khiyyantānaṁ vipācentānaṁ.",
  "pli-tv-bi-vb-pc32:1.6_0": "Yā tā bhikkhuniyo appicchā …pe… tā ujjhāyanti khiyyanti vipācenti—",
  "pli-tv-bi-vb-pc32:1.7_0": "“kathañhi nāma bhikkhuniyo dve ekattharaṇapāvuraṇā tuvaṭṭessantī”ti …pe…",
  "pli-tv-bi-vb-pc32:1.8_0": "“saccaṁ kira, bhikkhave,",
  "pli-tv-bi-vb-pc32:1.8_1": "bhikkhuniyo dve ekattharaṇapāvuraṇā tuvaṭṭentī”ti?",
  "pli-tv-bi-vb-pc32:1.9_0": "“Saccaṁ, bhagavā”ti.",
  "pli-tv-bi-vb-pc32:1.10_0": "Vigarahi buddho bhagavā …pe…",
  "pli-tv-bi-vb-pc32:1.11_0": "kathañhi nāma, bhikkhave,",
  "pli-tv-bi-vb-pc32:1.11_1": "bhikkhuniyo dve ekattharaṇapāvuraṇā tuvaṭṭessanti.",
  "pli-tv-bi-vb-pc32:1.12_0": "Netaṁ, bhikkhave, appasannānaṁ vā pasādāya …pe…",
  "pli-tv-bi-vb-pc32:1.13_0": "evañca pana, bhikkhave,",
  "pli-tv-bi-vb-pc32:1.13_1": "bhikkhuniyo imaṁ sikkhāpadaṁ uddisantu—",
  "pli-tv-bi-vb-pc32:1.14.1_0": "“Yā pana bhikkhuniyo dve ekattharaṇapāvuraṇā tuvaṭṭeyyuṁ, pācittiyan”ti.",
  "pli-tv-bi-vb-pc32:2.1.1_0": "Yā panāti",
  "pli-tv-bi-vb-pc32:2.1.2_0": "yā yādisā …pe…",
  "pli-tv-bi-vb-pc32:2.1.3_0": "bhikkhuniyoti",
  "pli-tv-bi-vb-pc32:2.1.4_0": "upasampannāyo vuccanti.",
  "pli-tv-bi-vb-pc32:2.1.5_0": "Dve ekattharaṇapāvuraṇā tuvaṭṭeyyunti",
  "pli-tv-bi-vb-pc32:2.1.6_0": "taññeva attharitvā taññeva pārupanti, āpatti pācittiyassa.",
  "pli-tv-bi-vb-pc32:2.2.1_0": "Ekattharaṇapāvuraṇe ekattharaṇapāvuraṇasaññā tuvaṭṭenti, āpatti pācittiyassa.",
  "pli-tv-bi-vb-pc32:2.2.2_0": "Ekattharaṇapāvuraṇe vematikā tuvaṭṭenti, āpatti pācittiyassa.",
  "pli-tv-bi-vb-pc32:2.2.3_0": "Ekattharaṇapāvuraṇe nānattharaṇapāvuraṇasaññā tuvaṭṭenti, āpatti pācittiyassa.",
  "pli-tv-bi-vb-pc32:2.2.4_0": "Ekattharaṇe nānāpāvuraṇe, āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc32:2.2.5_0": "Nānattharaṇe ekapāvuraṇe, āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc32:2.2.6_0": "Nānattharaṇapāvuraṇe ekattharaṇapāvuraṇasaññā, āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc32:2.2.7_0": "Nānattharaṇapāvuraṇe vematikā, āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc32:2.2.8_0": "Nānattharaṇapāvuraṇe nānattharaṇapāvuraṇasaññā, anāpatti.",
  "pli-tv-bi-vb-pc32:2.3.1_0": "Anāpatti—",
  "pli-tv-bi-vb-pc32:2.3.2_0": "vavatthānaṁ dassetvā nipajjanti,",
  "pli-tv-bi-vb-pc32:2.3.3_0": "ummattikānaṁ,",
  "pli-tv-bi-vb-pc32:2.3.4_0": "ādikammikānanti.",
  "pli-tv-bi-vb-pc32:2.3.5_0": "Dutiyasikkhāpadaṁ niṭṭhitaṁ."
}