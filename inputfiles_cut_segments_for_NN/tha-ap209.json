{
  "tha-ap209:0.1_0": "Therāpadāna",
  "tha-ap209:0.2_0": "Kaṇikārapupphiyavagga",
  "tha-ap209:0.3_0": "7 Salalamāliyattheraapadāna",
  "tha-ap209:1.1_0": "“Kaṇikāraṁva jotantaṁ,",
  "tha-ap209:1.2_0": "nisinnaṁ pabbatantare;",
  "tha-ap209:1.3_0": "Obhāsentaṁ disā sabbā,",
  "tha-ap209:1.4_0": "siddhatthaṁ narasārathiṁ.",
  "tha-ap209:2.1_0": "Dhanuṁ advejjhaṁ katvāna,",
  "tha-ap209:2.2_0": "usuṁ sannayhahaṁ tadā;",
  "tha-ap209:2.3_0": "Pupphaṁ savaṇṭaṁ chetvāna,",
  "tha-ap209:2.4_0": "buddhassa abhiropayiṁ.",
  "tha-ap209:3.1_0": "Catunnavutito kappe,",
  "tha-ap209:3.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap209:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap209:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap209:4.1_0": "Ekapaññāsito kappe,",
  "tha-ap209:4.2_0": "eko āsiṁ jutindharo;",
  "tha-ap209:4.3_0": "Sattaratanasampanno,",
  "tha-ap209:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap209:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap209:5.2_0": "…pe…",
  "tha-ap209:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap209:6.1_0": "Itthaṁ sudaṁ āyasmā salalamāliyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap209:7.1_0": "Salalamāliyattherassāpadānaṁ sattamaṁ."
}