{
  "iti57:0.1_0": "Itivuttaka 57",
  "iti57:0.2_0": "Tikanipāta",
  "iti57:0.3_0": "Paṭhamavagga",
  "iti57:0.4_0": "Dutiyaāsavasutta",
  "iti57:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti57:2.1_0": "“Tayome, bhikkhave, āsavā.",
  "iti57:2.2_0": "Katame tayo?",
  "iti57:2.3_0": "Kāmāsavo, bhavāsavo, avijjāsavo—",
  "iti57:2.4_0": "ime kho, bhikkhave, tayo āsavā”ti.",
  "iti57:2.5_0": "Etamatthaṁ bhagavā avoca.",
  "iti57:2.6_0": "Tatthetaṁ iti vuccati:",
  "iti57:3.1_0": "“Yassa kāmāsavo khīṇo,",
  "iti57:3.2_0": "avijjā ca virājitā;",
  "iti57:3.3_0": "Bhavāsavo parikkhīṇo,",
  "iti57:3.4_0": "vippamutto nirūpadhi;",
  "iti57:3.5_0": "Dhāreti antimaṁ dehaṁ,",
  "iti57:3.6_0": "jetvā māraṁ savāhinin”ti.",
  "iti57:4.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti57:4.2_0": "Aṭṭhamaṁ."
}