{
  "kv3.12:0.1_0": "Kathāvatthu",
  "kv3.12:0.2_0": "Mahāpaṇṇāsaka",
  "kv3.12:0.3_0": "Tatiyavagga",
  "kv3.12:0.4_0": "Nevasaññānāsaññāyatanakathā",
  "kv3.12:1.1_0": "Nevasaññānāsaññāyatane na vattabbaṁ—",
  "kv3.12:1.2_0": "“saññā atthī”ti?",
  "kv3.12:1.3_0": "Āmantā.",
  "kv3.12:1.4_0": "Asaññabhavo asaññagati asaññasattāvāso asaññasaṁsāro asaññayoni asaññattabhāvapaṭilābhoti?",
  "kv3.12:1.5_0": "Na hevaṁ vattabbe …pe….",
  "kv3.12:2.1_0": "Nanu saññābhavo saññāgati saññāsattāvāso saññāsaṁsāro saññāyoni saññattabhāvapaṭilābhoti?",
  "kv3.12:2.2_0": "Āmantā.",
  "kv3.12:2.3_0": "Hañci saññābhavo saññāgati …pe…",
  "kv3.12:2.4_0": "saññattabhāvapaṭilābho, no ca vata re vattabbe—",
  "kv3.12:2.5_0": "“nevasaññānāsaññāyatane na vattabbaṁ—",
  "kv3.12:2.6_0": "‘saññā atthī’”ti.",
  "kv3.12:3.1_0": "Nevasaññānāsaññāyatane na vattabbaṁ—",
  "kv3.12:3.2_0": "“saññā atthī”ti?",
  "kv3.12:3.3_0": "Āmantā.",
  "kv3.12:3.4_0": "Ekavokārabhavo gati …pe…",
  "kv3.12:3.5_0": "attabhāvapaṭilābhoti?",
  "kv3.12:3.6_0": "Na hevaṁ vattabbe …pe….",
  "kv3.12:4.1_0": "Nanu catuvokārabhavo gati …pe…",
  "kv3.12:4.2_0": "attabhāvapaṭilābhoti?",
  "kv3.12:4.3_0": "Āmantā.",
  "kv3.12:4.4_0": "Hañci catuvokārabhavo gati …pe…",
  "kv3.12:4.5_0": "attabhāvapaṭilābho, no ca vata re vattabbe—",
  "kv3.12:4.6_0": "“nevasaññānāsaññāyatane na vattabbaṁ—",
  "kv3.12:4.7_0": "‘saññā atthī’”ti.",
  "kv3.12:5.1_0": "Asaññasattesu na vattabbaṁ—",
  "kv3.12:5.2_0": "“saññā atthi”,",
  "kv3.12:5.2_1": "so ca asaññabhavo asaññagati asaññasattāvāso asaññasaṁsāro asaññayoni asaññattabhāvapaṭilābhoti?",
  "kv3.12:5.3_0": "Āmantā.",
  "kv3.12:5.4_0": "Nevasaññānāsaññāyatane na vattabbaṁ—",
  "kv3.12:5.5_0": "“saññā atthi”,",
  "kv3.12:5.5_1": "so ca asaññabhavo asaññagati asaññasattāvāso asaññasaṁsāro asaññayoni asaññattabhāvapaṭilābhoti?",
  "kv3.12:5.6_0": "Na hevaṁ vattabbe …pe….",
  "kv3.12:6.1_0": "Asaññasattesu na vattabbaṁ—",
  "kv3.12:6.2_0": "“saññā atthi”, so ca ekavokārabhavo gati …pe…",
  "kv3.12:6.3_0": "attabhāvapaṭilābhoti?",
  "kv3.12:6.4_0": "Āmantā.",
  "kv3.12:6.5_0": "Nevasaññānāsaññāyatane na vattabbaṁ—",
  "kv3.12:6.6_0": "“saññā atthi”,",
  "kv3.12:6.6_1": "so ca ekavokārabhavo gati sattāvāso saṁsāro yoni attabhāvapaṭilābhoti?",
  "kv3.12:6.7_0": "Na hevaṁ vattabbe …pe….",
  "kv3.12:7.1_0": "Nevasaññānāsaññāyatane na vattabbaṁ—",
  "kv3.12:7.2_0": "“saññā atthi”, so ca saññābhavo saññāgati …pe…",
  "kv3.12:7.3_0": "saññattabhāvapaṭilābhoti?",
  "kv3.12:7.4_0": "Āmantā.",
  "kv3.12:7.5_0": "Asaññasattesu na vattabbaṁ—",
  "kv3.12:7.6_0": "“saññā atthi”, so ca saññābhavo saññāgati …pe…",
  "kv3.12:7.7_0": "saññattabhāvapaṭilābhoti?",
  "kv3.12:7.8_0": "Na hevaṁ vattabbe …pe…",
  "kv3.12:8.1_0": "Nevasaññānāsaññāyatane na vattabbaṁ—",
  "kv3.12:8.2_0": "“saññā atthi”, so ca catuvokārabhavo gati …pe…",
  "kv3.12:8.3_0": "attabhāvapaṭilābhoti?",
  "kv3.12:8.4_0": "Āmantā.",
  "kv3.12:8.5_0": "Asaññasattesu na vattabbaṁ—",
  "kv3.12:8.6_0": "“saññā atthi”, so ca catuvokārabhavo gati …pe…",
  "kv3.12:8.7_0": "attabhāvapaṭilābhoti?",
  "kv3.12:8.8_0": "Na hevaṁ vattabbe …pe….",
  "kv3.12:9.1_0": "Nevasaññānāsaññāyatane na vattabbaṁ—",
  "kv3.12:9.2_0": "“saññā atthī”ti?",
  "kv3.12:9.3_0": "Āmantā.",
  "kv3.12:9.4_0": "Nanu nevasaññānāsaññāyatanaṁ catuvokārabhavoti?",
  "kv3.12:9.5_0": "Āmantā.",
  "kv3.12:9.6_0": "Hañci nevasaññānāsaññāyatanaṁ catuvokārabhavo, no ca vata re vattabbe—",
  "kv3.12:9.7_0": "“nevasaññānāsaññāyatane na vattabbaṁ—",
  "kv3.12:9.8_0": "‘saññā atthī’”ti.",
  "kv3.12:10.1_0": "Nevasaññānāsaññāyatanaṁ catuvokārabhavo nevasaññānāsaññāyatane na vattabbaṁ—",
  "kv3.12:10.2_0": "“saññā atthī”ti?",
  "kv3.12:10.3_0": "Āmantā.",
  "kv3.12:10.4_0": "Ākāsānañcāyatanaṁ catuvokārabhavo ākāsānañcāyatane na vattabbaṁ—",
  "kv3.12:10.5_0": "“saññā atthī”ti?",
  "kv3.12:10.6_0": "Na hevaṁ vattabbe …pe….",
  "kv3.12:11.1_0": "Nevasaññānāsaññāyatanaṁ catuvokārabhavo,",
  "kv3.12:11.1_1": "nevasaññānāsaññāyatane na vattabbaṁ—",
  "kv3.12:11.2_0": "“saññā atthī”ti?",
  "kv3.12:11.3_0": "Āmantā.",
  "kv3.12:11.4_0": "Viññāṇañcāyatanaṁ …pe…",
  "kv3.12:11.5_0": "ākiñcaññāyatanaṁ catuvokārabhavo, ākiñcaññāyatane na vattabbaṁ—",
  "kv3.12:11.6_0": "“saññā atthī”ti?",
  "kv3.12:11.7_0": "Na hevaṁ vattabbe …pe….",
  "kv3.12:12.1_0": "Ākāsānañcāyatanaṁ catuvokārabhavo, atthi tattha saññāti?",
  "kv3.12:12.2_0": "Āmantā.",
  "kv3.12:12.3_0": "Nevasaññānāsaññāyatanaṁ catuvokārabhavo, atthi tattha saññāti?",
  "kv3.12:12.4_0": "Na hevaṁ vattabbe …pe….",
  "kv3.12:13.1_0": "Viññāṇañcāyatanaṁ …pe…",
  "kv3.12:13.2_0": "ākiñcaññāyatanaṁ catuvokārabhavo, atthi tattha saññāti?",
  "kv3.12:13.3_0": "Āmantā.",
  "kv3.12:13.4_0": "Nevasaññānāsaññāyatanaṁ catuvokārabhavo, atthi tattha saññāti?",
  "kv3.12:13.5_0": "Na hevaṁ vattabbe …pe….",
  "kv3.12:14.1_0": "Nevasaññānāsaññāyatane na vattabbaṁ—",
  "kv3.12:14.2_0": "“saññā atthī”ti vā “natthī”ti vāti?",
  "kv3.12:14.3_0": "Āmantā.",
  "kv3.12:14.4_0": "Nanu nevasaññānāsaññāyatanaṁ catuvokārabhavoti?",
  "kv3.12:14.5_0": "Āmantā.",
  "kv3.12:14.6_0": "Hañci nevasaññānāsaññāyatanaṁ catuvokārabhavo, no ca vata re vattabbe—",
  "kv3.12:14.7_0": "“nevasaññānāsaññāyatane na vattabbaṁ—",
  "kv3.12:14.8_0": "‘saññā atthī’ti vā ‘natthī’ti vā”ti.",
  "kv3.12:15.1_0": "Nevasaññānāsaññāyatanaṁ catuvokārabhavo,",
  "kv3.12:15.1_1": "nevasaññānāsaññāyatane na vattabbaṁ—",
  "kv3.12:15.2_0": "“saññā atthī”ti vā “natthī”ti vāti?",
  "kv3.12:15.3_0": "Āmantā.",
  "kv3.12:15.4_0": "Ākāsānañcāyatanaṁ …pe…",
  "kv3.12:15.5_0": "viññāṇañcāyatanaṁ …pe…",
  "kv3.12:15.6_0": "ākiñcaññāyatanaṁ catuvokārabhavo, ākiñcaññāyatane na vattabbaṁ—",
  "kv3.12:15.7_0": "“saññā atthī”ti vā “natthī”ti vāti?",
  "kv3.12:15.8_0": "Na hevaṁ vattabbe …pe….",
  "kv3.12:16.1_0": "Ākāsānañcāyatanaṁ catuvokārabhavo, atthi tattha saññāti?",
  "kv3.12:16.2_0": "Āmantā.",
  "kv3.12:16.3_0": "Nevasaññānāsaññāyatanaṁ catuvokārabhavo, atthi tattha saññāti?",
  "kv3.12:16.4_0": "Na hevaṁ vattabbe …pe….",
  "kv3.12:17.1_0": "Viññāṇañcāyatanaṁ …pe…",
  "kv3.12:17.2_0": "ākiñcaññāyatanaṁ catuvokārabhavo, atthi tattha saññāti?",
  "kv3.12:17.3_0": "Āmantā.",
  "kv3.12:17.4_0": "Nevasaññānāsaññāyatanaṁ catuvokārabhavo, atthi tattha saññāti?",
  "kv3.12:17.5_0": "Na hevaṁ vattabbe …pe….",
  "kv3.12:18.1_0": "Nevasaññānāsaññāyatane na vattabbaṁ—",
  "kv3.12:18.2_0": "“saññā atthī”ti vā “natthī”ti vāti?",
  "kv3.12:18.3_0": "Āmantā.",
  "kv3.12:18.4_0": "Nanu nevasaññānāsaññāyatananti?",
  "kv3.12:18.5_0": "Āmantā.",
  "kv3.12:18.6_0": "Hañci nevasaññānāsaññāyatanaṁ, tena vata re vattabbe—",
  "kv3.12:18.7_0": "“nevasaññānāsaññāyatane na vattabbaṁ—",
  "kv3.12:18.8_0": "‘saññā atthī’ti vā ‘natthī’ti vā”ti.",
  "kv3.12:19.1_0": "Nevasaññānāsaññāyatananti katvā nevasaññānāsaññāyatane na vattabbaṁ—",
  "kv3.12:19.2_0": "“saññā atthī”ti vā “natthī”ti vāti?",
  "kv3.12:19.3_0": "Āmantā.",
  "kv3.12:19.4_0": "Adukkhamasukhā vedanāti katvā adukkhamasukhāya vedanāya na vattabbaṁ—",
  "kv3.12:19.5_0": "“vedanā”ti vā “avedanā”ti vāti?",
  "kv3.12:19.6_0": "Na hevaṁ vattabbe …pe….",
  "kv3.12:19.7_0": "Nevasaññānāsaññāyatanakathā niṭṭhitā.",
  "kv3.12:19.8_0": "Tatiyo vaggo.",
  "kv3.12:19.9_0": "Tassuddānaṁ",
  "kv3.12:20.1_0": "Balaṁ sādhāraṇaṁ ariyaṁ,",
  "kv3.12:20.2_0": "sarāgaṁ cittaṁ vimuccati;",
  "kv3.12:20.3_0": "Vimuttaṁ vimuccamānaṁ,",
  "kv3.12:20.4_0": "atthi cittaṁ vimuccamānaṁ.",
  "kv3.12:21.1_0": "Aṭṭhamakassa puggalassa,",
  "kv3.12:21.2_0": "Diṭṭhipariyuṭṭhānaṁ pahīnaṁ;",
  "kv3.12:21.3_0": "Aṭṭhamakassa puggalassa,",
  "kv3.12:21.4_0": "Natthi pañcindriyāni cakkhuṁ.",
  "kv3.12:22.1_0": "Sotaṁ dhammupatthaddhaṁ,",
  "kv3.12:22.2_0": "Yathākammūpagataṁ ñāṇaṁ;",
  "kv3.12:22.3_0": "Devesu saṁvaro asañña-",
  "kv3.12:22.4_0": "Sattesu saññā evameva bhavagganti."
}