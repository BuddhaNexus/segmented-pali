{
  "kv5.7:0.1_0": "Kathāvatthu",
  "kv5.7:0.2_0": "Mahāpaṇṇāsaka",
  "kv5.7:0.3_0": "Pañcamavagga",
  "kv5.7:0.4_0": "Cittārammaṇakathā",
  "kv5.7:1.1_0": "Cetopariyāye ñāṇaṁ cittārammaṇaññeva na aññārammaṇanti?",
  "kv5.7:1.2_0": "Āmantā.",
  "kv5.7:1.3_0": "Nanu atthi koci sarāgaṁ cittaṁ “sarāgaṁ cittan”ti pajānātīti?",
  "kv5.7:1.4_0": "Āmantā.",
  "kv5.7:1.5_0": "Hañci atthi koci sarāgaṁ cittaṁ “sarāgaṁ cittan”ti pajānāti, no ca vata re vattabbe—",
  "kv5.7:1.6_0": "“cetopariyāye ñāṇaṁ cittārammaṇaññeva na aññārammaṇan”ti.",
  "kv5.7:2.1_0": "Nanu atthi koci vītarāgaṁ cittaṁ …pe…",
  "kv5.7:2.2_0": "sadosaṁ cittaṁ …",
  "kv5.7:2.3_0": "vītadosaṁ cittaṁ …",
  "kv5.7:2.4_0": "samohaṁ cittaṁ …",
  "kv5.7:2.5_0": "vītamohaṁ cittaṁ …",
  "kv5.7:2.6_0": "saṅkhittaṁ cittaṁ …",
  "kv5.7:2.7_0": "vikkhittaṁ cittaṁ …",
  "kv5.7:2.8_0": "mahaggataṁ cittaṁ …",
  "kv5.7:2.9_0": "amahaggataṁ cittaṁ …",
  "kv5.7:2.10_0": "sauttaraṁ cittaṁ …",
  "kv5.7:2.11_0": "anuttaraṁ cittaṁ …",
  "kv5.7:2.12_0": "samāhitaṁ cittaṁ …",
  "kv5.7:2.13_0": "asamāhitaṁ cittaṁ …",
  "kv5.7:2.14_0": "vimuttaṁ cittaṁ …pe…",
  "kv5.7:2.15_0": "avimuttaṁ cittaṁ “avimuttaṁ cittan”ti pajānātīti?",
  "kv5.7:2.16_0": "Āmantā.",
  "kv5.7:2.17_0": "Hañci atthi koci avimuttaṁ cittaṁ “avimuttaṁ cittan”ti pajānāti, no ca vata re vattabbe—",
  "kv5.7:2.18_0": "“cetopariyāye ñāṇaṁ cittārammaṇaññeva na aññārammaṇan”ti.",
  "kv5.7:3.1_0": "Phassārammaṇe ñāṇaṁ vattabbaṁ—",
  "kv5.7:3.2_0": "“cetopariyāye ñāṇan”ti?",
  "kv5.7:3.3_0": "Āmantā.",
  "kv5.7:3.4_0": "Hañci phassārammaṇe ñāṇaṁ vattabbaṁ cetopariyāye ñāṇaṁ, no ca vata re vattabbe—",
  "kv5.7:3.5_0": "“cetopariyāye ñāṇaṁ cittārammaṇaññeva na aññārammaṇan”ti.",
  "kv5.7:3.6_0": "Vedanārammaṇe ñāṇaṁ …pe…",
  "kv5.7:3.7_0": "saññārammaṇe ñāṇaṁ …",
  "kv5.7:3.8_0": "cetanārammaṇe ñāṇaṁ …",
  "kv5.7:3.9_0": "cittārammaṇe ñāṇaṁ …",
  "kv5.7:3.10_0": "saddhārammaṇe ñāṇaṁ …",
  "kv5.7:3.11_0": "vīriyārammaṇe ñāṇaṁ …",
  "kv5.7:3.12_0": "satārammaṇe ñāṇaṁ …",
  "kv5.7:3.13_0": "samādhārammaṇe ñāṇaṁ …",
  "kv5.7:3.14_0": "paññārammaṇe ñāṇaṁ …",
  "kv5.7:3.15_0": "rāgārammaṇe ñāṇaṁ …",
  "kv5.7:3.16_0": "dosārammaṇe ñāṇaṁ …",
  "kv5.7:3.17_0": "mohārammaṇe ñāṇaṁ …pe…",
  "kv5.7:3.18_0": "anottappārammaṇe ñāṇaṁ vattabbaṁ—",
  "kv5.7:3.19_0": "“cetopariyāye ñāṇan”ti?",
  "kv5.7:3.20_0": "Āmantā.",
  "kv5.7:3.21_0": "Hañci anottappārammaṇe ñāṇaṁ vattabbaṁ cetopariyāye ñāṇaṁ, no ca vata re vattabbe—",
  "kv5.7:3.22_0": "“cetopariyāye ñāṇaṁ cittārammaṇaññeva na aññārammaṇan”ti.",
  "kv5.7:4.1_0": "Phassārammaṇe ñāṇaṁ na vattabbaṁ—",
  "kv5.7:4.2_0": "“cetopariyāye ñāṇan”ti?",
  "kv5.7:4.3_0": "Āmantā.",
  "kv5.7:4.4_0": "Phassapariyāye ñāṇanti?",
  "kv5.7:4.5_0": "Na hevaṁ vattabbe …pe…",
  "kv5.7:4.6_0": "vedanārammaṇe ñāṇaṁ …pe…",
  "kv5.7:4.7_0": "saññārammaṇe ñāṇaṁ …pe…",
  "kv5.7:4.8_0": "anottappārammaṇe ñāṇaṁ na vattabbaṁ—",
  "kv5.7:4.9_0": "“cetopariyāye ñāṇan”ti?",
  "kv5.7:4.10_0": "Āmantā.",
  "kv5.7:4.11_0": "Anottappapariyāye ñāṇanti?",
  "kv5.7:4.12_0": "Na hevaṁ vattabbe …pe….",
  "kv5.7:5.1_0": "Na vattabbaṁ—",
  "kv5.7:5.2_0": "“cetopariyāye ñāṇaṁ cittārammaṇaññeva na aññārammaṇan”ti?",
  "kv5.7:5.3_0": "Āmantā.",
  "kv5.7:5.4_0": "Nanu cetopariyāye ñāṇanti?",
  "kv5.7:5.5_0": "Āmantā.",
  "kv5.7:5.6_0": "Hañci cetopariyāye ñāṇaṁ, tena vata re vattabbe—",
  "kv5.7:5.7_0": "“cetopariyāye ñāṇaṁ cittārammaṇaññeva na aññārammaṇan”ti.",
  "kv5.7:5.8_0": "Cittārammaṇakathā niṭṭhitā."
}