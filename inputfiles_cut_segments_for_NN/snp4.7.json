{
  "snp4.7:0.1_0": "Sutta Nipāta 4.7",
  "snp4.7:0.2_0": "Tissametteyyasutta",
  "snp4.7:1.1_0": "“Methunamanuyuttassa,",
  "snp4.7:1.2_0": "(iccāyasmā tisso metteyyo)",
  "snp4.7:1.3_0": "Vighātaṁ brūhi mārisa;",
  "snp4.7:1.4_0": "Sutvāna tava sāsanaṁ,",
  "snp4.7:1.5_0": "Viveke sikkhissāmase”.",
  "snp4.7:2.1_0": "“Methunamanuyuttassa,",
  "snp4.7:2.2_0": "(metteyyāti bhagavā)",
  "snp4.7:2.3_0": "Mussate vāpi sāsanaṁ;",
  "snp4.7:2.4_0": "Micchā ca paṭipajjati,",
  "snp4.7:2.5_0": "Etaṁ tasmiṁ anāriyaṁ.",
  "snp4.7:3.1_0": "Eko pubbe caritvāna,",
  "snp4.7:3.2_0": "methunaṁ yo nisevati;",
  "snp4.7:3.3_0": "Yānaṁ bhantaṁva taṁ loke,",
  "snp4.7:3.4_0": "hīnamāhu puthujjanaṁ.",
  "snp4.7:4.1_0": "Yaso kitti ca yā pubbe,",
  "snp4.7:4.2_0": "hāyate vāpi tassa sā;",
  "snp4.7:4.3_0": "Etampi disvā sikkhetha,",
  "snp4.7:4.4_0": "methunaṁ vippahātave.",
  "snp4.7:5.1_0": "Saṅkappehi pareto so,",
  "snp4.7:5.2_0": "kapaṇo viya jhāyati;",
  "snp4.7:5.3_0": "Sutvā paresaṁ nigghosaṁ,",
  "snp4.7:5.4_0": "maṅku hoti tathāvidho.",
  "snp4.7:6.1_0": "Atha satthāni kurute,",
  "snp4.7:6.2_0": "paravādehi codito;",
  "snp4.7:6.3_0": "Esa khvassa mahāgedho,",
  "snp4.7:6.4_0": "mosavajjaṁ pagāhati.",
  "snp4.7:7.1_0": "Paṇḍitoti samaññāto,",
  "snp4.7:7.2_0": "ekacariyaṁ adhiṭṭhito;",
  "snp4.7:7.3_0": "Athāpi methune yutto,",
  "snp4.7:7.4_0": "mandova parikissati.",
  "snp4.7:8.1_0": "Etamādīnavaṁ ñatvā,",
  "snp4.7:8.2_0": "Muni pubbāpare idha;",
  "snp4.7:8.3_0": "Ekacariyaṁ daḷhaṁ kayirā,",
  "snp4.7:8.4_0": "Na nisevetha methunaṁ.",
  "snp4.7:9.1_0": "Vivekaññeva sikkhetha,",
  "snp4.7:9.2_0": "etaṁ ariyānamuttamaṁ;",
  "snp4.7:9.3_0": "Na tena seṭṭho maññetha,",
  "snp4.7:9.4_0": "sa ve nibbānasantike.",
  "snp4.7:10.1_0": "Rittassa munino carato,",
  "snp4.7:10.2_0": "Kāmesu anapekkhino;",
  "snp4.7:10.3_0": "Oghatiṇṇassa pihayanti,",
  "snp4.7:10.4_0": "Kāmesu gadhitā pajā”ti.",
  "snp4.7:10.5_0": "Tissametteyyasuttaṁ sattamaṁ."
}