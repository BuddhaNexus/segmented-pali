{
  "mil3.5.1:0.1_0": "Milindapañha",
  "mil3.5.1:0.2_0": "Buddhavagga",
  "mil3.5.1:0.3_0": "1. Buddhassaatthinatthibhāvapañha",
  "mil3.5.1:1.1_0": "Rājā āha—",
  "mil3.5.1:1.2_0": "“bhante nāgasena, buddho tayā diṭṭho”ti?",
  "mil3.5.1:1.3_0": "“Na hi, mahārājā”ti.",
  "mil3.5.1:1.4_0": "“Atha te ācariyehi buddho diṭṭho”ti?",
  "mil3.5.1:1.5_0": "“Na hi, mahārājā”ti.",
  "mil3.5.1:1.6_0": "“Tena hi, bhante nāgasena, natthi buddho”ti.",
  "mil3.5.1:1.7_0": "“Kiṁ pana, mahārāja, himavati ūhā nadī tayā diṭṭhā”ti?",
  "mil3.5.1:1.8_0": "“Na hi, bhante”ti.",
  "mil3.5.1:1.9_0": "“Atha te pitarā ūhā nadī diṭṭhā”ti?",
  "mil3.5.1:1.10_0": "“Na hi, bhante”ti.",
  "mil3.5.1:1.11_0": "“Tena hi, mahārāja, natthi ūhā nadī”ti.",
  "mil3.5.1:1.12_0": "“Atthi, bhante,",
  "mil3.5.1:1.12_1": "kiñcāpi mayā ūhā nadī na diṭṭhā,",
  "mil3.5.1:1.12_2": "pitarāpi me ūhā nadī na diṭṭhā,",
  "mil3.5.1:1.12_3": "api ca atthi ūhā nadī”ti.",
  "mil3.5.1:1.13_0": "“Evameva kho, mahārāja,",
  "mil3.5.1:1.13_1": "kiñcāpi mayā bhagavā na diṭṭho,",
  "mil3.5.1:1.13_2": "ācariyehipi me bhagavā na diṭṭho,",
  "mil3.5.1:1.13_3": "api ca atthi bhagavā”ti.",
  "mil3.5.1:2.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.5.1:3.1_0": "Buddhassa atthinatthibhāvapañho paṭhamo."
}