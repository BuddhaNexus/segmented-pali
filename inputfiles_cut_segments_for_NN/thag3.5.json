{
  "thag3.5:0.1_0": "Theragāthā",
  "thag3.5:0.2_0": "Tikanipāta",
  "thag3.5:0.3_0": "Paṭhamavagga",
  "thag3.5:0.4_0": "3.5. Mātaṅgaputtattheragāthā",
  "thag3.5:1.1_0": "“Atisītaṁ atiuṇhaṁ,",
  "thag3.5:1.2_0": "atisāyamidaṁ ahu;",
  "thag3.5:1.3_0": "Iti vissaṭṭhakammante,",
  "thag3.5:1.4_0": "khaṇā accenti māṇave.",
  "thag3.5:2.1_0": "Yo ca sītañca uṇhañca,",
  "thag3.5:2.2_0": "tiṇā bhiyyo na maññati;",
  "thag3.5:2.3_0": "Karaṁ purisakiccāni,",
  "thag3.5:2.4_0": "so sukhā na vihāyati.",
  "thag3.5:3.1_0": "Dabbaṁ kusaṁ poṭakilaṁ,",
  "thag3.5:3.2_0": "usīraṁ muñjapabbajaṁ;",
  "thag3.5:3.3_0": "Urasā panudissāmi,",
  "thag3.5:3.4_0": "vivekamanubrūhayan”ti.",
  "thag3.5:3.5_0": "… Mātaṅgaputto thero …."
}