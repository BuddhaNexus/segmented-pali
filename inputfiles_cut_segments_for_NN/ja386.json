{
  "ja386:0.1_0": "Jātaka",
  "ja386:0.2_0": "Chakkanipāta",
  "ja386:0.3_0": "Kharaputtavagga",
  "ja386:0.4_0": "1. Kharaputtajātaka",
  "ja386:1.1_0": "“Saccaṁ kirevamāhaṁsu,",
  "ja386:1.2_0": "vastaṁ bāloti paṇḍitā;",
  "ja386:1.3_0": "Passa bālo rahokammaṁ,",
  "ja386:1.4_0": "āvikubbaṁ na bujjhati”.",
  "ja386:2.1_0": "“Tvaṁ khopi samma bālosi,",
  "ja386:2.2_0": "kharaputta vijānahi;",
  "ja386:2.3_0": "Rajjuyā hi parikkhitto,",
  "ja386:2.4_0": "vaṅkoṭṭho ohitomukho.",
  "ja386:3.1_0": "Aparampi samma te bālyaṁ,",
  "ja386:3.2_0": "yo mutto na palāyasi;",
  "ja386:3.3_0": "So ca bālataro samma,",
  "ja386:3.4_0": "yaṁ tvaṁ vahasi senakaṁ”.",
  "ja386:4.1_0": "“Yannu samma ahaṁ bālo,",
  "ja386:4.2_0": "ajarāja vijānahi;",
  "ja386:4.3_0": "Atha kena senako bālo,",
  "ja386:4.4_0": "taṁ me akkhāhi pucchito”.",
  "ja386:5.1_0": "“Uttamatthaṁ labhitvāna,",
  "ja386:5.2_0": "bhariyāya yo padassati;",
  "ja386:5.3_0": "Tena jahissatattānaṁ,",
  "ja386:5.4_0": "sā cevassa na hessati”.",
  "ja386:6.1_0": "“Na ve piyammeti janinda tādiso,",
  "ja386:6.2_0": "Attaṁ niraṅkatvā piyāni sevati;",
  "ja386:6.3_0": "Attāva seyyo paramā ca seyyo,",
  "ja386:6.4_0": "Labbhā piyā ocitatthena pacchā”ti.",
  "ja386:7.1_0": "Kharaputtajātakaṁ paṭhamaṁ."
}