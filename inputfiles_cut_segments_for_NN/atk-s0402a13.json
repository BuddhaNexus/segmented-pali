{
  "atk-s0402a13:0_0": "(14) 4. Santhāravaggavaṇṇanā",
  "atk-s0402a13:1_0": "152.",
  "atk-s0402a13:1_1": "Catutthassa paṭhame catūhi paccayehi attano ca parassa ca antarapaṭicchādanavasena santharaṇaṁ āmisasanthāro,",
  "atk-s0402a13:1_2": "dhammena santharaṇaṁ dhammasanthāro. Dutiye upasaggamattaṁ viseso.",
  "atk-s0402a13:2_0": "154.",
  "atk-s0402a13:2_1": "Tatiye vuttappakārassa āmisassa esanā āmisesanā, dhammassa esanā dhammesanā.",
  "atk-s0402a13:2_2": "Catutthe upasaggamattameva viseso.",
  "atk-s0402a13:3_0": "156.",
  "atk-s0402a13:3_1": "Pañcame matthakappattā āmisapariyesanā āmisapariyeṭṭhi,",
  "atk-s0402a13:3_2": "matthakappattāva dhammapariyesanā dhammapariyeṭṭhīti vuttā.",
  "atk-s0402a13:4_0": "157.",
  "atk-s0402a13:4_1": "Chaṭṭhe āmisena pūjanaṁ āmisapūjā, dhammena pūjanaṁ dhammapūjā.",
  "atk-s0402a13:5_0": "158.",
  "atk-s0402a13:5_1": "Sattame ātitheyyānīti āgantukadānāni. Atitheyyānītipi pāṭho.",
  "atk-s0402a13:6_0": "159.",
  "atk-s0402a13:6_1": "Aṭṭhame āmisaṁ ijjhanakasamijjhanakavasena āmisiddhi,",
  "atk-s0402a13:6_2": "dhammopi ijjhanakasamijjhanakavasena dhammiddhi.",
  "atk-s0402a13:7_0": "160.",
  "atk-s0402a13:7_1": "Navame āmisena vaḍḍhanaṁ āmisavuddhi,",
  "atk-s0402a13:7_2": "dhammena vaḍḍhanaṁ dhammavuddhi.",
  "atk-s0402a13:8_0": "161.",
  "atk-s0402a13:8_1": "Dasame ratikaraṇaṭṭhena āmisaṁ āmisaratanaṁ, dhammo dhammaratanaṁ.",
  "atk-s0402a13:10_0": "162.",
  "atk-s0402a13:10_1": "Ekādasame āmisassa cinanaṁ vaḍḍhanaṁ āmisasannicayo,",
  "atk-s0402a13:10_2": "dhammassa cinanaṁ vaḍḍhanaṁ dhammasannicayo.",
  "atk-s0402a13:11_0": "163.",
  "atk-s0402a13:11_1": "Dvādasame āmisassa vipulabhāvo āmisavepullaṁ,",
  "atk-s0402a13:11_2": "dhammassa vipulabhāvo dhammavepullanti.",
  "atk-s0402a13:12_0": "Santhāravaggo catuttho."
}