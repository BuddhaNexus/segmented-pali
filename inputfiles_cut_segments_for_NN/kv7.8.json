{
  "kv7.8:0.1_0": "Kathāvatthu",
  "kv7.8:0.2_0": "Mahāpaṇṇāsaka",
  "kv7.8:0.3_0": "Sattamavagga",
  "kv7.8:0.4_0": "Jarāmaraṇaṁvipākotikathā",
  "kv7.8:1.1_0": "Jarāmaraṇaṁ vipākoti?",
  "kv7.8:1.2_0": "Āmantā.",
  "kv7.8:1.3_0": "Sukhavedaniyaṁ dukkhavedaniyaṁ adukkhamasukhavedaniyaṁ,",
  "kv7.8:1.3_1": "sukhāya vedanāya sampayuttaṁ,",
  "kv7.8:1.3_2": "dukkhāya vedanāya sampayuttaṁ,",
  "kv7.8:1.3_3": "adukkhamasukhāya vedanāya sampayuttaṁ, phassena sampayuttaṁ, vedanāya sampayuttaṁ, saññāya sampayuttaṁ, cetanāya sampayuttaṁ, cittena sampayuttaṁ, sārammaṇaṁ;",
  "kv7.8:1.4_0": "atthi tassa āvaṭṭanā ābhogo samannāhāro manasikāro cetanā patthanā paṇidhīti?",
  "kv7.8:1.5_0": "Na hevaṁ vattabbe …pe…",
  "kv7.8:1.6_0": "nanu na sukhavedaniyaṁ na dukkhavedaniyaṁ …pe… anārammaṇaṁ;",
  "kv7.8:1.7_0": "natthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv7.8:1.8_0": "Āmantā.",
  "kv7.8:1.9_0": "Hañci na sukhavedaniyaṁ na dukkhavedaniyaṁ …pe… anārammaṇaṁ;",
  "kv7.8:1.10_0": "natthi tassa āvaṭṭanā …pe… paṇidhi, no ca vata re vattabbe—",
  "kv7.8:1.11_0": "“jarāmaraṇaṁ vipāko”ti.",
  "kv7.8:2.1_0": "Phasso vipāko,",
  "kv7.8:2.1_1": "phasso sukhavedaniyo dukkhavedaniyo …pe…",
  "kv7.8:2.2_0": "sārammaṇo;",
  "kv7.8:2.3_0": "atthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv7.8:2.4_0": "Āmantā.",
  "kv7.8:2.5_0": "Jarāmaraṇaṁ vipāko,",
  "kv7.8:2.5_1": "jarāmaraṇaṁ sukhavedaniyaṁ dukkhavedaniyaṁ …pe…",
  "kv7.8:2.6_0": "sārammaṇaṁ;",
  "kv7.8:2.7_0": "atthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv7.8:2.8_0": "Na hevaṁ vattabbe …pe….",
  "kv7.8:3.1_0": "Jarāmaraṇaṁ vipāko,",
  "kv7.8:3.1_1": "jarāmaraṇaṁ na sukhavedaniyaṁ na dukkhavedaniyaṁ …pe… anārammaṇaṁ;",
  "kv7.8:3.2_0": "natthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv7.8:3.3_0": "Āmantā.",
  "kv7.8:3.4_0": "Phasso vipāko,",
  "kv7.8:3.4_1": "phasso na sukhavedaniyo na dukkhavedaniyo …pe… anārammaṇo;",
  "kv7.8:3.5_0": "natthi tassa āvaṭṭanā …pe… paṇidhīti?",
  "kv7.8:3.6_0": "Na hevaṁ vattabbe …pe….",
  "kv7.8:4.1_0": "Akusalānaṁ dhammānaṁ jarāmaraṇaṁ,",
  "kv7.8:4.1_1": "akusalānaṁ dhammānaṁ vipākoti?",
  "kv7.8:4.2_0": "Āmantā.",
  "kv7.8:4.3_0": "Kusalānaṁ dhammānaṁ jarāmaraṇaṁ, kusalānaṁ dhammānaṁ vipākoti?",
  "kv7.8:4.4_0": "Na hevaṁ vattabbe …pe….",
  "kv7.8:5.1_0": "Kusalānaṁ dhammānaṁ jarāmaraṇaṁ, na vattabbaṁ—",
  "kv7.8:5.2_0": "“kusalānaṁ dhammānaṁ vipāko”ti?",
  "kv7.8:5.3_0": "Āmantā.",
  "kv7.8:5.4_0": "Akusalānaṁ dhammānaṁ jarāmaraṇaṁ, na vattabbaṁ—",
  "kv7.8:5.5_0": "“akusalānaṁ dhammānaṁ vipāko”ti?",
  "kv7.8:5.6_0": "Na hevaṁ vattabbe …pe….",
  "kv7.8:6.1_0": "Kusalānaṁ dhammānaṁ jarāmaraṇaṁ,",
  "kv7.8:6.1_1": "akusalānaṁ dhammānaṁ vipākoti?",
  "kv7.8:6.2_0": "Āmantā.",
  "kv7.8:6.3_0": "Akusalānaṁ dhammānaṁ jarāmaraṇaṁ, kusalānaṁ dhammānaṁ vipākoti?",
  "kv7.8:6.4_0": "Na hevaṁ vattabbe …pe….",
  "kv7.8:7.1_0": "Akusalānaṁ dhammānaṁ jarāmaraṇaṁ, na vattabbaṁ—",
  "kv7.8:7.2_0": "“kusalānaṁ dhammānaṁ vipāko”ti?",
  "kv7.8:7.3_0": "Āmantā.",
  "kv7.8:7.4_0": "Kusalānaṁ dhammānaṁ jarāmaraṇaṁ, na vattabbaṁ—",
  "kv7.8:7.5_0": "“akusalānaṁ dhammānaṁ vipāko”ti?",
  "kv7.8:7.6_0": "Na hevaṁ vattabbe …pe….",
  "kv7.8:8.1_0": "Kusalānañca akusalānañca dhammānaṁ jarāmaraṇaṁ,",
  "kv7.8:8.1_1": "akusalānaṁ dhammānaṁ vipākoti?",
  "kv7.8:8.2_0": "Āmantā.",
  "kv7.8:8.3_0": "Kusalānañca akusalānañca dhammānaṁ jarāmaraṇaṁ, kusalānaṁ dhammānaṁ vipākoti?",
  "kv7.8:8.4_0": "Na hevaṁ vattabbe …pe….",
  "kv7.8:9.1_0": "Kusalānañca akusalānañca dhammānaṁ jarāmaraṇaṁ, na vattabbaṁ—",
  "kv7.8:9.2_0": "“kusalānaṁ dhammānaṁ vipāko”ti?",
  "kv7.8:9.3_0": "Āmantā.",
  "kv7.8:9.4_0": "Kusalānañca akusalānañca dhammānaṁ jarāmaraṇaṁ, na vattabbaṁ—",
  "kv7.8:9.5_0": "“akusalānaṁ dhammānaṁ vipāko”ti?",
  "kv7.8:9.6_0": "Na hevaṁ vattabbe …pe….",
  "kv7.8:10.1_0": "Na vattabbaṁ—",
  "kv7.8:10.2_0": "“jarāmaraṇaṁ vipāko”ti?",
  "kv7.8:10.3_0": "Āmantā.",
  "kv7.8:10.4_0": "Nanu atthi dubbaṇṇasaṁvattaniyaṁ kammaṁ appāyukasaṁvattaniyaṁ kammanti?",
  "kv7.8:10.5_0": "Āmantā.",
  "kv7.8:10.6_0": "Hañci atthi dubbaṇṇasaṁvattaniyaṁ kammaṁ appāyukasaṁvattaniyaṁ kammaṁ, tena vata re vattabbe—",
  "kv7.8:10.7_0": "“jarāmaraṇaṁ vipāko”ti.",
  "kv7.8:10.8_0": "Jarāmaraṇaṁ vipākotikathā niṭṭhitā."
}