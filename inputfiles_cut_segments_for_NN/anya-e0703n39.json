{
  "anya-e0703n39:0_0": "Cattālīsatima pariccheda",
  "anya-e0703n39:1_0": "Aṭṭharājako",
  "anya-e0703n39:2_0": "1.",
  "anya-e0703n39:3_0": " Tassaccaye kumārādi-dhātusenoti vissuto;",
  "anya-e0703n39:4_0": "Ahu tassa suto rājā, devarūpo mahābalo.",
  "anya-e0703n39:5_0": "2.",
  "anya-e0703n39:6_0": " Kārite pitarā’kāsi, vihāre navakammakaṁ;",
  "anya-e0703n39:7_0": "Kāretvā dhammasaṅgītiṁ parisodheti sāsanaṁ.",
  "anya-e0703n39:8_0": "3.",
  "anya-e0703n39:9_0": " Santappesi mahāsaṅghaṁ, paccayehi catūhipi;",
  "anya-e0703n39:10_0": "Katvā puññāni’nekāni, navame hāyane’tigā.",
  "anya-e0703n39:11_0": "4.",
  "anya-e0703n39:12_0": " Tittiseno suto tassa, rājā hutvā anekadhā;",
  "anya-e0703n39:13_0": "Katvā puññāni rajjaṁ taṁ, māsampi navame jahi.",
  "anya-e0703n39:14_0": "5.",
  "anya-e0703n39:15_0": " Sivo taṁ mātulo hantvā, hutvā rājā anappakaṁ;",
  "anya-e0703n39:16_0": "Puññaṁ katvo’patissena, pañcavīsa dine hato.",
  "anya-e0703n39:17_0": "6.",
  "anya-e0703n39:18_0": " Upatisso tato āsi, rājā hantvāna sīvakaṁ;",
  "anya-e0703n39:19_0": "Moggallānassa bhaginī, sāmiko dhajinīpati.",
  "anya-e0703n39:20_0": "7.",
  "anya-e0703n39:21_0": " Rājā ṭhānantarādīhi, katvāna janasaṅgahaṁ;",
  "anya-e0703n39:22_0": "Sīlākāḷassa pādāsi, saha bho kanadhīvaraṁ.",
  "anya-e0703n39:23_0": "8.",
  "anya-e0703n39:24_0": " Eko putto ahu rañño, upatissassa kassapo;",
  "anya-e0703n39:25_0": "Sasoḷasa sahāyehi, sūro sūpehi saññuto.",
  "anya-e0703n39:26_0": "9.",
  "anya-e0703n39:27_0": " Eka vuttisahāyehi, dānamāna mahādhano;",
  "anya-e0703n39:28_0": "Dhammaṭṭho vīriyājīvi, sādhu jeṭṭhapacāyako.",
  "anya-e0703n39:29_0": "10.",
  "anya-e0703n39:30_0": " Silā kāḷotato rajja-lobhavañcita mānaso;",
  "anya-e0703n39:31_0": "Dakkhiṇaṁ malayaṁ gantvā, saṅgaṇhitvā mahā balaṁ.",
  "anya-e0703n39:32_0": "11.",
  "anya-e0703n39:33_0": " Vilumpamāno paccanti, sampatto nagarantikaṁ;",
  "anya-e0703n39:34_0": "Taṁ sutvā kassapo jeṭṭho, varamāruyha kuñjaraṁ.",
  "anya-e0703n39:35_0": "12.",
  "anya-e0703n39:36_0": " Assā setvāna pitaraṁ, samādāya sahāyake;",
  "anya-e0703n39:37_0": "Nikkhamma nagarā gacchi, silākālassa dassanaṁ.",
  "anya-e0703n39:38_0": "13.",
  "anya-e0703n39:39_0": " Evaṁ satta’ṭṭha vāresu, palāto līnavuttiko;",
  "anya-e0703n39:40_0": "Hatthe katvā upāyena, dese pācina pacchime.",
  "anya-e0703n39:41_0": "14.",
  "anya-e0703n39:42_0": " Yujjhituṁ puna pācina-tissa pabbatamāgami;",
  "anya-e0703n39:43_0": "Kassapopi sahāyehi, saddhimāruyha dantinaṁ.",
  "anya-e0703n39:44_0": "15.",
  "anya-e0703n39:45_0": " Tattha gantvā palāpetvā, coraṁ pabbatamatthakaṁ;",
  "anya-e0703n39:46_0": "Āropesi mahānāgaṁ, tenā’si girikassapo.",
  "anya-e0703n39:47_0": "16.",
  "anya-e0703n39:48_0": " Mānatthaddho silākāḷo, bhiyyo raṭṭhaṁ pabhindiya;",
  "anya-e0703n39:49_0": "Sabbaṁ hatthagataṁ katvā, ajeyya balavāhano.",
  "anya-e0703n39:50_0": "17.",
  "anya-e0703n39:51_0": " Āgamma nagaraṁ rundhi, satthāhaṁ rājasevakā;",
  "anya-e0703n39:52_0": "Yujjhitvā viralā āsuṁ, tato cintesi kassapo.",
  "anya-e0703n39:53_0": "18.",
  "anya-e0703n39:54_0": " Ete nagararodhena, sabbe bhijjanti pāṇino;",
  "anya-e0703n39:55_0": "Parihīnaṁ balaṁ rājā, andhako ca mahallako.",
  "anya-e0703n39:56_0": "19.",
  "anya-e0703n39:57_0": " Merukandarake katvā, mātaraṁ pitarañca me;",
  "anya-e0703n39:58_0": "Aṅgahetvā balaṁ pacchā, coro niggaṇhi yo iti.",
  "anya-e0703n39:59_0": "20.",
  "anya-e0703n39:60_0": " Rattiyaṁ so sahāye ca, rājasādhanamevaca;",
  "anya-e0703n39:61_0": "Ādāya pitaro ceva, malayaṁ gantumārabhi.",
  "anya-e0703n39:62_0": "21.",
  "anya-e0703n39:63_0": " Tadā maggamajānantā, sammūḷā maggadesakā;",
  "anya-e0703n39:64_0": "Nagarassa samīpeva, sambhamiṁsu ito tato.",
  "anya-e0703n39:65_0": "22.",
  "anya-e0703n39:66_0": " Silākāḷo pavattiṁ taṁ, sutvā saṅgamma vegasā;",
  "anya-e0703n39:67_0": "Parivāresi saṅgāmo, tattha bhiṁsanako ahu.",
  "anya-e0703n39:68_0": "23.",
  "anya-e0703n39:69_0": " Devāsuraraṇākāre, vattamāne mahāhave;",
  "anya-e0703n39:70_0": "Patitesu sahāyesu, sīdamāne mahāgaje.",
  "anya-e0703n39:71_0": "24.",
  "anya-e0703n39:72_0": " Hatthārohassa datvā, chinditvā sīsamattano;",
  "anya-e0703n39:73_0": "Puñchitvā lohitaṁ katvā, kosiyaṁ asi puttikaṁ.",
  "anya-e0703n39:74_0": "25.",
  "anya-e0703n39:75_0": " Hatthikumbhe ubho hatthe, ṭhapetvāna avatthari;",
  "anya-e0703n39:76_0": "Upatissopi taṁ sutvā, sokasallāhato mari.",
  "anya-e0703n39:77_0": "26.",
  "anya-e0703n39:78_0": " Evaṁ diyaḍḍhavassena, upatisse divaṁgate;",
  "anya-e0703n39:79_0": "Rājā’hosi silākāḷo, pubbanāmena ekato.",
  "anya-e0703n39:80_0": "27.",
  "anya-e0703n39:81_0": " Taṁ ambasāmaṇerādi-silākāḷoti voharuṁ;",
  "anya-e0703n39:82_0": "Titthaṁ terasavassāni, dipaṁ dhammena pālayi.",
  "anya-e0703n39:83_0": "28.",
  "anya-e0703n39:84_0": " Mahāpāḷimhi dāpesi, paccagghaṁ rājabhojanaṁ;",
  "anya-e0703n39:85_0": "Vejjasālāsu bhoge ca, vaḍḍhesi janatāhito.",
  "anya-e0703n39:86_0": "29.",
  "anya-e0703n39:87_0": " Anvahaṁ pūjayi bodhiṁ, paṭimāyo ca kārayi;",
  "anya-e0703n39:88_0": "Sabbesaṁ dīpavāsīnaṁ, bhikkhūnaṁ’dā ticīvaraṁ.",
  "anya-e0703n39:89_0": "30.",
  "anya-e0703n39:90_0": " Māghātaṁ kārayidīpe, sabbesaṁyeva pāṇinaṁ;",
  "anya-e0703n39:91_0": "Ānitaṁ attanā kesa-dhātuṁ sammā apūjayi.",
  "anya-e0703n39:92_0": "31.",
  "anya-e0703n39:93_0": " Raheradakavārañca, adāsi abhayuttare;",
  "anya-e0703n39:94_0": "Puratthimā therīyānaṁ, vihārakunthanāma so.",
  "anya-e0703n39:95_0": "32.",
  "anya-e0703n39:96_0": " Ānetvā āsanaṁ tattha, ṭhapesi dumarājake;",
  "anya-e0703n39:97_0": "Yāvajīvaṁ pavattesi, puññakammamasaṅkhiyaṁ.",
  "anya-e0703n39:98_0": "33.",
  "anya-e0703n39:99_0": " Moggallāno tathā dāṭhā, pabhuti co’patissako;",
  "anya-e0703n39:100_0": "Putto tassā’sumaggassa, desaṁ datvā puratthimaṁ.",
  "anya-e0703n39:101_0": "34.",
  "anya-e0703n39:102_0": " Datvā ṭhānantarañcādi-pādasaññaṁ visajjayi;",
  "anya-e0703n39:103_0": "Gantvā tattha vasāhīti, sopi gantvā tahiṁ vasi.",
  "anya-e0703n39:104_0": "35.",
  "anya-e0703n39:105_0": " Ṭhānaṁ malayarājaggaṁ, desaṁ datvāna dakkhiṇaṁ;",
  "anya-e0703n39:106_0": "Rakkhaṇatthaṁ samuddassa, majjhimaṁ tu niyojayi.",
  "anya-e0703n39:107_0": "36.",
  "anya-e0703n39:108_0": " Upatissaṁ tu vāsesi, santikeyeva attano;",
  "anya-e0703n39:109_0": "Visesena mamāyanto, yūnaṁ kalyānadassanaṁ.",
  "anya-e0703n39:110_0": "37.",
  "anya-e0703n39:111_0": " Tassa dvādasame vasse, ito kāsi puraṁ gato;",
  "anya-e0703n39:112_0": "Dhammātu midhā’nesi, tato vāṇija māṇavo.",
  "anya-e0703n39:113_0": "38.",
  "anya-e0703n39:114_0": " Rājā disvā’samattho so, dhammādhammavicāraṇe;",
  "anya-e0703n39:115_0": "Hemasaññāya dīpamhi, patanto salabho viya.",
  "anya-e0703n39:116_0": "39.",
  "anya-e0703n39:117_0": " Buddhadhammoti saññāya, taṁ gahetvāna sādhukaṁ;",
  "anya-e0703n39:118_0": "Katvā sakkārasammānaṁ, gehe rājagharantike.",
  "anya-e0703n39:119_0": "40.",
  "anya-e0703n39:120_0": " Ṭhapetvā anuvassaṁ tu, netvā jetavanaṁ mahaṁ;",
  "anya-e0703n39:121_0": "Kātuṁ kāresi cārittaṁ, hitaṁ mantvāna pāṇinaṁ.",
  "anya-e0703n39:122_0": "41.",
  "anya-e0703n39:123_0": " Evaṁ katvā silākāḷo, puññakammamanappakaṁ;",
  "anya-e0703n39:124_0": "Patte terasame vasse, yathākammamupāgami.",
  "anya-e0703n39:125_0": "42.",
  "anya-e0703n39:126_0": " Dāṭhappabhutiko rajjaṁ, gahetvā bhātaraṁsakaṁ;",
  "anya-e0703n39:127_0": "Akkamoti nivārentaṁ, mārāpesi vibuddhiko.",
  "anya-e0703n39:128_0": "43.",
  "anya-e0703n39:129_0": " Moggallāno’tha taṁ sutvā, appattaṁ rajjamaggahi;",
  "anya-e0703n39:130_0": "Akāraṇe me māresi, kaṇiṭṭhaṁ dhammavādīnaṁ.",
  "anya-e0703n39:131_0": "44.",
  "anya-e0703n39:132_0": " Kārāpessāmahampajja, rajjanti parikuppiya;",
  "anya-e0703n39:133_0": "Samādāya mahāsenaṁ, agārāhera pabbataṁ.",
  "anya-e0703n39:134_0": "45.",
  "anya-e0703n39:135_0": " Rājāpi sutvā sannaddha-balakāyo karindake;",
  "anya-e0703n39:136_0": "Pabbate siviraṁ bandhi, moggallāno nisammataṁ.",
  "anya-e0703n39:137_0": "46.",
  "anya-e0703n39:138_0": " Sāparādhāna te me vā, manussā dīpavāsino;",
  "anya-e0703n39:139_0": "Ekasmiñca mate rajja-mubhinnaṁyeva nosiyā.",
  "anya-e0703n39:140_0": "47.",
  "anya-e0703n39:141_0": " Tasmā aññena yujjhantu, ubhoyeva mayaṁ idha;",
  "anya-e0703n39:142_0": "Hatthiyuddhaṁ karomāti, rañño pesesi sāsanaṁ.",
  "anya-e0703n39:143_0": "48.",
  "anya-e0703n39:144_0": " Sopi sādhūti vatvāna, baddhapañcāyudho gajaṁ;",
  "anya-e0703n39:145_0": "Āruyha munino māro viya otthari tāvade.",
  "anya-e0703n39:146_0": "49.",
  "anya-e0703n39:147_0": " Moggallānopi sannaddho, āruyha karīnaṁ varaṁ;",
  "anya-e0703n39:148_0": "Tatthā’go aññamaññaṁ te, pāpuṇiṁsu mahāgajā.",
  "anya-e0703n39:149_0": "50.",
  "anya-e0703n39:150_0": " Saddo sūyittha saṅghaṭṭe, asanirāva sannibho;",
  "anya-e0703n39:151_0": "Dantaghātena uṭṭhāsi, jālā vijjullatā viya.",
  "anya-e0703n39:152_0": "51.",
  "anya-e0703n39:153_0": " Sañjhāghanasabhāgā’suṁ, gajā lohitamakkhitā;",
  "anya-e0703n39:154_0": "Moggallānagajāviddho, raññoosakki kuñjaro.",
  "anya-e0703n39:155_0": "52.",
  "anya-e0703n39:156_0": " Rājā ārabhi taṁ disvā, chindituṁ sīsamattano;",
  "anya-e0703n39:157_0": "Moggallāno’tha vandanto, yāci’me’vaṁ kirīiti.",
  "anya-e0703n39:158_0": "53.",
  "anya-e0703n39:159_0": " Yācamānepi somānaṁ, mānento chindikandharaṁ;",
  "anya-e0703n39:160_0": "Chaḍḍesi chahi so rajjaṁ, māsehi divasehi ca.",
  "anya-e0703n39:161_0": "54.",
  "anya-e0703n39:162_0": " Moggallāno tato rājā, āsi dīpe mahābalo;",
  "anya-e0703n39:163_0": "Mātulañca paṭiccemaṁ, cūlanāmena voharuṁ.",
  "anya-e0703n39:164_0": "55.",
  "anya-e0703n39:165_0": " Āsādhāraṇakāveyyo, vatthuttaya parāyaṇo;",
  "anya-e0703n39:166_0": "Dānasaṁyama soceyyo, soraccādiguṇālayo.",
  "anya-e0703n39:167_0": "56.",
  "anya-e0703n39:168_0": " Dānena piyavācāya, atthassa cariyāya ca;",
  "anya-e0703n39:169_0": "Samānattassabhāvena, saṅgahesi mahājanaṁ.",
  "anya-e0703n39:170_0": "57.",
  "anya-e0703n39:171_0": " Piṇḍapātavihārehi, bhesajjacchādanehi ca;",
  "anya-e0703n39:172_0": "Bhikkhusaṅghañhi saṅgaṇhi, dhammikāya ca guttiyā.",
  "anya-e0703n39:173_0": "58.",
  "anya-e0703n39:174_0": " Atirekāya pūjāya, pūjetvā dhammabhāṇake;",
  "anya-e0703n39:175_0": "Piṭake tīṇi vācesi, saddhimaṭṭhakathāya so.",
  "anya-e0703n39:176_0": "59.",
  "anya-e0703n39:177_0": " Kumāre upalāletvā, nivāpena yathāruciṁ;",
  "anya-e0703n39:178_0": "Sajjhāpesi sadā dhammaṁ, dhammadīpo mahāmati.",
  "anya-e0703n39:179_0": "60.",
  "anya-e0703n39:180_0": " Dhammadīpañca so katvā, kuñjarasekhareni sā;",
  "anya-e0703n39:181_0": "Dhammāvāsāne vācesi, puramhi purisuttamo.",
  "anya-e0703n39:182_0": "61.",
  "anya-e0703n39:183_0": " Bandhāpesi kadambañca, nadiṁpabbatamajjhato;",
  "anya-e0703n39:184_0": "Pattapasāṇavāpiñca, dhanavāpiṁ garitaraṁ.",
  "anya-e0703n39:185_0": "62.",
  "anya-e0703n39:186_0": " Gaṇhāpesi sadīghāyu-hetu kammanti sādaro;",
  "anya-e0703n39:187_0": "Likhāpesi ca saddhammaṁ, vatthupūjañca kārayi.",
  "anya-e0703n39:188_0": "63.",
  "anya-e0703n39:189_0": " Lokaṁ so anukampitvā, mātāputtaṁva orasaṁ;",
  "anya-e0703n39:190_0": "Datvā bhutvā yathākāmaṁ, vasse vīsatime mari.",
  "anya-e0703n39:191_0": "64.",
  "anya-e0703n39:192_0": " Mahesī tassa ghātetvā, visayogena ñātake;",
  "anya-e0703n39:193_0": "Puttaṁ rajje’bhisiñcitvā, sayaṁ rajjaṁ vicārayi.",
  "anya-e0703n39:194_0": "65.",
  "anya-e0703n39:195_0": " Tathābhisitto so kitti-sirimegho narādhipo;",
  "anya-e0703n39:196_0": "Tipupattehi chādesi, dumindadharamādito.",
  "anya-e0703n39:197_0": "66.",
  "anya-e0703n39:198_0": " Kapaṇaddhivaṇibbānaṁ, mahādānaṁ pavattayi;",
  "anya-e0703n39:199_0": "Maggapālo tathākāro, ahu sabbopabhogiyo.",
  "anya-e0703n39:200_0": "67.",
  "anya-e0703n39:201_0": " Mahesī sā sadā āsi, padhānā sabbakammasu;",
  "anya-e0703n39:202_0": "Rajjaṁ tassā’si teneva, heṭṭhupariyavattikaṁ.",
  "anya-e0703n39:203_0": "68.",
  "anya-e0703n39:204_0": " Rājāpādā mahāmaccā’-hesuṁ lañcaparāyanā;",
  "anya-e0703n39:205_0": "Dubbale ca viheṭhesuṁ, balī jānapadā narā.",
  "anya-e0703n39:206_0": "69.",
  "anya-e0703n39:207_0": " Silākāḷassa kālamhi, gāme saṅgillanāmake;",
  "anya-e0703n39:208_0": "Bhayavasīvhayo poso, ahu moriyavaṁsajo.",
  "anya-e0703n39:209_0": "70.",
  "anya-e0703n39:210_0": " Ahosi putto sīvassa, aggabodhi sanāmako;",
  "anya-e0703n39:211_0": "Bhāgineyyopi tassāsi, mahānāgoti vissuto.",
  "anya-e0703n39:212_0": "71.",
  "anya-e0703n39:213_0": " Bhāgineyo mahānāgo, aggabodhi ca sundaro;",
  "anya-e0703n39:214_0": "Uḷārajjhāsayattā so, mahānāgo mahabbalo.",
  "anya-e0703n39:215_0": "72.",
  "anya-e0703n39:216_0": " Hitvā kassakakammāni, corakammamakā vane;",
  "anya-e0703n39:217_0": "Godhaṁ laddhāna pesesi, mātulāniya santikaṁ.",
  "anya-e0703n39:218_0": "73.",
  "anya-e0703n39:219_0": " Godhaṁ disvā’vasā ñatvā, dhaññapacchimapesayi;",
  "anya-e0703n39:220_0": "Kammārassā’pi pesesi, sasaṁ sopi tathevakā.",
  "anya-e0703n39:221_0": "74.",
  "anya-e0703n39:222_0": " Bījaṁ bhaginī māyāci, bījagāhañca tassa sā;",
  "anya-e0703n39:223_0": "Dāsañca ñatvā pesesi, annapānādinā raho.",
  "anya-e0703n39:224_0": "75.",
  "anya-e0703n39:225_0": " Tadā dubbhikkhakālamhi, eko mantadharo naro;",
  "anya-e0703n39:226_0": "Bhikkhālābhāya saddhehi, bhikkhuvesena bhikkhati.",
  "anya-e0703n39:227_0": "76.",
  "anya-e0703n39:228_0": " Taṁ gāmaṁ pavisitvā so, aladdhā kiñci bhojanaṁ;",
  "anya-e0703n39:229_0": "Abhibhūto jighacchāya, kampamāno nigacchati.",
  "anya-e0703n39:230_0": "77.",
  "anya-e0703n39:231_0": " Taṁ disvā karuṇāyanto, mahānāgo mahādayo;",
  "anya-e0703n39:232_0": "Pattamādāya gāmanta-māhiṇḍitvāpi sabbaso.",
  "anya-e0703n39:233_0": "78.",
  "anya-e0703n39:234_0": " Yāgumattampi nālattha, tato uttarasāṭakaṁ;",
  "anya-e0703n39:235_0": "Datvā āhari āhāraṁ, so taṁ bhutvā pasīdiya.",
  "anya-e0703n39:236_0": "79.",
  "anya-e0703n39:237_0": " Rajjārahamimaṁ dīpe, karissāmīti cintiya;",
  "anya-e0703n39:238_0": "Tamādāya khaṇenā’gā, gokaṇṇakamahaṇṇavaṁ.",
  "anya-e0703n39:239_0": "80.",
  "anya-e0703n39:240_0": " Atha tattha nisīditvā, sañjapanto yathāvidhiṁma;",
  "anya-e0703n39:241_0": "Mantonā’nesi nāgindaṁ, phussapuṇṇamarattiyaṁ.",
  "anya-e0703n39:242_0": "81.",
  "anya-e0703n39:243_0": " Mahānāgaṁ phusāhīti, mahānāgaṁ niyojayi;",
  "anya-e0703n39:244_0": "So bhīto purime yāme, āgataṁ taṁ na sambhusī.",
  "anya-e0703n39:245_0": "82.",
  "anya-e0703n39:246_0": " Tathā majjhimayāmepi, pacchime pana naṅgale;",
  "anya-e0703n39:247_0": "Gahetvā khipi tīheva, aṅgulīhi sataṁ chupi.",
  "anya-e0703n39:248_0": "83.",
  "anya-e0703n39:249_0": " So taṁ byākāsi taṁ ditvā, sabalaṁ me parissamaṁ;",
  "anya-e0703n39:250_0": "Tīhi rājūhi yujjhitvā, catutthaṁ tvaṁ nighātiya.",
  "anya-e0703n39:251_0": "84.",
  "anya-e0703n39:252_0": " Vuḍḍho tīṇeva vassāni, rājā hutvā na jīvasi;",
  "anya-e0703n39:253_0": "Tathā hessanti rājāno, tayo te vaṁsajā narā.",
  "anya-e0703n39:254_0": "85.",
  "anya-e0703n39:255_0": " Gantvā sevassu rājānaṁ, pacchā passasi mekhalaṁ;",
  "anya-e0703n39:256_0": "Iti vatvāna pesesi, sopi gantvā narissaraṁ.",
  "anya-e0703n39:257_0": "86.",
  "anya-e0703n39:258_0": " Passitvā tamupaṭṭhāsi, rājā rohaṇakammikaṁ;",
  "anya-e0703n39:259_0": "Taṁ akāsi taduṭṭhānaṁ, bhaṇḍamāhari so bahuṁ.",
  "anya-e0703n39:260_0": "87.",
  "anya-e0703n39:261_0": " Rājā tasmiṁ pasīditvā, andhasenāpativhayaṁ;",
  "anya-e0703n39:262_0": "Datvā ṭhānantaraṁ tassa, gantuṁ tattheva yojayi.",
  "anya-e0703n39:263_0": "88.",
  "anya-e0703n39:264_0": " Bhayasīvassa puttañca, bhāgineyyañca attano;",
  "anya-e0703n39:265_0": "Ādāya gantvā taṁ desaṁ, parivattesi sabbaso.",
  "anya-e0703n39:266_0": "89.",
  "anya-e0703n39:267_0": " Paccekabhogaṁ katvāna, rohaṇaṁ tattha so vasaṁ;",
  "anya-e0703n39:268_0": "Dāṭhappabhūtinā kātuṁ, yuddhaṁgantvā mahabbalo.",
  "anya-e0703n39:269_0": "90.",
  "anya-e0703n39:270_0": " Moggallānabhayā gantvā, rohaṇañca tahiṁ vasī;",
  "anya-e0703n39:271_0": "Sutvā kittisirīmeghavaṇṇa-rañño rajje samañjasaṁ.",
  "anya-e0703n39:272_0": "91.",
  "anya-e0703n39:273_0": " Rajjaṁ gahetuṁ kāloti, sīghaṁ āgamma rohaṇā;",
  "anya-e0703n39:274_0": "Ekūnavise divase, mārayitvā mahīpatiṁ.",
  "anya-e0703n39:275_0": "92.",
  "anya-e0703n39:276_0": " Sayaṁ hutvā mahīpālo, desaṁ katvā yathā pure;",
  "anya-e0703n39:277_0": "Bhāgineyyassa pāhesi, paṇṇamāgacchatūti so.",
  "anya-e0703n39:278_0": "93.",
  "anya-e0703n39:279_0": " Āgacchanto nimittena, nivattitvā marittha so;",
  "anya-e0703n39:280_0": "Tato mātulaputtaṁ’kā, uparajjaṁ kataññuko.",
  "anya-e0703n39:281_0": "94.",
  "anya-e0703n39:282_0": " Ālavālaṁ dumindassa, katvā hemamayaṁ gharaṁ;",
  "anya-e0703n39:283_0": "Chādāpesi munindassa, paṭimāyo ca sandahi.",
  "anya-e0703n39:284_0": "95.",
  "anya-e0703n39:285_0": " Mahācetittaye kāsi, sudhākammañca cumbaṭaṁ;",
  "anya-e0703n39:286_0": "Hatthivediñca kāretvā, cittakammamakārayi.",
  "anya-e0703n39:287_0": "96.",
  "anya-e0703n39:288_0": " Pesakārakagāmaṁ so, jambelavhayamuttare;",
  "anya-e0703n39:289_0": "Mahāvihārecābandhi, gāmaṁ tintiṇikavhayaṁ.",
  "anya-e0703n39:290_0": "97.",
  "anya-e0703n39:291_0": " Uddhagāmamhi vasabha-gāmaṁ jetavanassa’dā;",
  "anya-e0703n39:292_0": "Vatthadānaṁ nikāyesu, tīsu ceva pavattayi.",
  "anya-e0703n39:293_0": "98.",
  "anya-e0703n39:294_0": " Khettānaṁ hisataṁ datvā, vihāre jetanāmake;",
  "anya-e0703n39:295_0": "Yāguṁ tattha pavattesi, bhikkhūnaṁ sabbakālikaṁ.",
  "anya-e0703n39:296_0": "99.",
  "anya-e0703n39:297_0": " Sahassa dūratissavhā, khettaṁ datvā tapassinaṁ;",
  "anya-e0703n39:298_0": "Mahāvihāravāsīnaṁ, yāguṁ niccaṁ pavattayi.",
  "anya-e0703n39:299_0": "100.",
  "anya-e0703n39:300_0": " Ciramātikavārañca, tattheva’dā guṇe rato;",
  "anya-e0703n39:301_0": "Mayūrapariveṇe ca, navakammamakārayi.",
  "anya-e0703n39:302_0": "101.",
  "anya-e0703n39:303_0": " Kāsikhaṇḍe mahādeva-rattakuravanāmake;",
  "anya-e0703n39:304_0": "Vihāre anurārāmaṁ, jiṇṇañca paṭisaṅkharī.",
  "anya-e0703n39:305_0": "102.",
  "anya-e0703n39:306_0": " Kamaṁ sovaggikaṁ katvā, evamādiṁ narissaro;",
  "anya-e0703n39:307_0": "Agamā tīhi vassehi, devarājasahabyataṁ.",
  "anya-e0703n39:308_0": "103.",
  "anya-e0703n39:309_0": " Aṭṭhete kuṭṭhacittā’parimitavibhavā rājarājenarūpā;",
  "anya-e0703n39:310_0": "Rājāno rājamānā narakarituragāsūrasenārathehi;",
  "anya-e0703n39:311_0": "Ante hitvā’khilaṁ taṁ vigataparijanā’ḷāhanaṁ saṅkhatāsuṁ;",
  "anya-e0703n39:312_0": "Sappañño taṁ saranto bhavatu bhavasukhaṁ vantukāmo hitesī.",
  "anya-e0703n39:313_0": "Sujanappasādasaṁvegatthāya kate mahāvaṁse",
  "anya-e0703n39:314_0": "Aṭṭharājako nāma",
  "anya-e0703n39:315_0": "Cattālīsatimo paricchedo."
}