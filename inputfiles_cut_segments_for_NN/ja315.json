{
  "ja315:0.1_0": "Jātaka",
  "ja315:0.2_0": "Catukkanipāta",
  "ja315:0.3_0": "Pucimandavagga",
  "ja315:0.4_0": "5. Sabbamaṁsalābhajātaka",
  "ja315:1.1_0": "“Pharusā vata te vācā,",
  "ja315:1.2_0": "maṁsaṁ yācanako asi;",
  "ja315:1.3_0": "Kilomasadisī vācā,",
  "ja315:1.4_0": "kilomaṁ samma dammi te”.",
  "ja315:2.1_0": "“Aṅgametaṁ manussānaṁ,",
  "ja315:2.2_0": "bhātā loke pavuccati;",
  "ja315:2.3_0": "Aṅgassa sadisī vācā,",
  "ja315:2.4_0": "aṅgaṁ samma dadāmi te”.",
  "ja315:3.1_0": "“Tātāti putto vadamāno,",
  "ja315:3.2_0": "kampeti hadayaṁ pitu;",
  "ja315:3.3_0": "Hadayassa sadisī vācā,",
  "ja315:3.4_0": "hadayaṁ samma dammi te”.",
  "ja315:4.1_0": "“Yassa gāme sakhā natthi,",
  "ja315:4.2_0": "yathāraññaṁ tatheva taṁ;",
  "ja315:4.3_0": "Sabbassa sadisī vācā,",
  "ja315:4.4_0": "sabbaṁ samma dadāmi te”ti.",
  "ja315:5.1_0": "Sabbamaṁsalābhajātakaṁ pañcamaṁ."
}