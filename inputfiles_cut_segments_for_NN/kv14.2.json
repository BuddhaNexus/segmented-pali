{
  "kv14.2:0.1_0": "Kathāvatthu",
  "kv14.2:0.2_0": "Mahāpaṇṇāsaka",
  "kv14.2:0.3_0": "Cuddasamavagga",
  "kv14.2:0.4_0": "Saḷāyatanuppattikathā",
  "kv14.2:1.1_0": "Saḷāyatanaṁ apubbaṁ acarimaṁ mātukucchismiṁ saṇṭhātīti?",
  "kv14.2:1.2_0": "Āmantā.",
  "kv14.2:1.3_0": "Sabbaṅgapaccaṅgī ahīnindriyo mātukucchismiṁ okkamatīti?",
  "kv14.2:1.4_0": "Na hevaṁ vattabbe …pe….",
  "kv14.2:2.1_0": "Upapattesiyena cittena cakkhāyatanaṁ saṇṭhātīti?",
  "kv14.2:2.2_0": "Āmantā.",
  "kv14.2:2.3_0": "Upapattesiyena cittena hatthā saṇṭhanti, pādā saṇṭhanti, sīsaṁ saṇṭhāti, kaṇṇo saṇṭhāti, nāsikā saṇṭhāti, mukhaṁ saṇṭhāti, dantā saṇṭhantīti?",
  "kv14.2:2.4_0": "Na hevaṁ vattabbe …pe….",
  "kv14.2:3.1_0": "Upapattesiyena cittena sotāyatanaṁ …pe…",
  "kv14.2:3.2_0": "ghānāyatanaṁ …pe…",
  "kv14.2:3.3_0": "jivhāyatanaṁ saṇṭhātīti?",
  "kv14.2:3.4_0": "Āmantā.",
  "kv14.2:3.5_0": "Upapattesiyena cittena hatthā saṇṭhanti, pādā saṇṭhanti, sīsaṁ saṇṭhāti, kaṇṇo saṇṭhāti, nāsikā saṇṭhāti, mukhaṁ saṇṭhāti, dantā saṇṭhantīti?",
  "kv14.2:3.6_0": "Na hevaṁ vattabbe …pe….",
  "kv14.2:4.1_0": "Mātukucchigatassa pacchā cakkhāyatanaṁ uppajjatīti?",
  "kv14.2:4.2_0": "Āmantā.",
  "kv14.2:4.3_0": "Mātukucchismiṁ cakkhupaṭilābhāya kammaṁ karotīti?",
  "kv14.2:4.4_0": "Na hevaṁ vattabbe …pe…",
  "kv14.2:4.5_0": "mātukucchigatassa pacchā sotāyatanaṁ …pe…",
  "kv14.2:4.6_0": "ghānāyatanaṁ …pe…",
  "kv14.2:4.7_0": "jivhāyatanaṁ uppajjatīti?",
  "kv14.2:4.8_0": "Āmantā.",
  "kv14.2:4.9_0": "Mātukucchismiṁ jivhāpaṭilābhāya kammaṁ karotīti?",
  "kv14.2:4.10_0": "Na hevaṁ vattabbe …pe….",
  "kv14.2:5.1_0": "Mātukucchigatassa pacchā kesā lomā nakhā dantā aṭṭhī uppajjantīti?",
  "kv14.2:5.2_0": "Āmantā.",
  "kv14.2:5.3_0": "Mātukucchismiṁ aṭṭhipaṭilābhāya kammaṁ karotīti?",
  "kv14.2:5.4_0": "Na hevaṁ vattabbe …pe….",
  "kv14.2:6.1_0": "Na vattabbaṁ—",
  "kv14.2:6.2_0": "“mātukucchigatassa pacchā kesā lomā nakhā dantā aṭṭhī uppajjantī”ti?",
  "kv14.2:6.3_0": "Āmantā.",
  "kv14.2:6.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv14.2:7.1_0": "“Paṭhamaṁ kalalaṁ hoti,",
  "kv14.2:7.2_0": "kalalā hoti abbudaṁ;",
  "kv14.2:7.3_0": "Abbudā jāyate pesi,",
  "kv14.2:7.4_0": "pesi nibbattate ghano;",
  "kv14.2:7.5_0": "Ghanā pasākhā jāyanti,",
  "kv14.2:7.6_0": "kesā lomā nakhāpi ca.",
  "kv14.2:8.1_0": "Yañcassa bhuñjati mātā,",
  "kv14.2:8.2_0": "annaṁ pānañca bhojanaṁ;",
  "kv14.2:8.3_0": "Tena so tattha yāpeti,",
  "kv14.2:8.4_0": "mātukucchigato naro”ti.",
  "kv14.2:9.1_0": "Attheva suttantoti?",
  "kv14.2:9.2_0": "Āmantā.",
  "kv14.2:9.3_0": "Tena hi mātukucchigatassa pacchā kesā lomā nakhā dantā aṭṭhī uppajjantīti.",
  "kv14.2:9.4_0": "Saḷāyatanuppattikathā niṭṭhitā."
}