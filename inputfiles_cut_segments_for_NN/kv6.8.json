{
  "kv6.8:0.1_0": "Kathāvatthu",
  "kv6.8:0.2_0": "Mahāpaṇṇāsaka",
  "kv6.8:0.3_0": "Chaṭṭhavagga",
  "kv6.8:0.4_0": "Pathavīdhātusanidassanātiādikathā",
  "kv6.8:1.1_0": "Pathavīdhātu sanidassanāti?",
  "kv6.8:1.2_0": "Āmantā.",
  "kv6.8:1.3_0": "Rūpaṁ rūpāyatanaṁ rūpadhātu nīlaṁ pītakaṁ lohitakaṁ odātaṁ cakkhuviññeyyaṁ cakkhusmiṁ paṭihaññati cakkhussa āpāthaṁ āgacchatīti?",
  "kv6.8:1.4_0": "Na hevaṁ vattabbe …pe….",
  "kv6.8:2.1_0": "Pathavīdhātu sanidassanāti?",
  "kv6.8:2.2_0": "Āmantā.",
  "kv6.8:2.3_0": "Cakkhuñca paṭicca pathavīdhātuñca uppajjati cakkhuviññāṇanti?",
  "kv6.8:2.4_0": "Na hevaṁ vattabbe …pe….",
  "kv6.8:3.1_0": "Cakkhuñca paṭicca pathavīdhātuñca uppajjati cakkhuviññāṇanti?",
  "kv6.8:3.2_0": "Āmantā.",
  "kv6.8:3.3_0": "“Cakkhuñca paṭicca pathavīdhātuñca uppajjati cakkhuviññāṇan”ti—",
  "kv6.8:3.4_0": "attheva suttantoti?",
  "kv6.8:3.5_0": "Natthi.",
  "kv6.8:3.6_0": "“Cakkhuñca paṭicca rūpe ca uppajjati cakkhuviññāṇan”ti—",
  "kv6.8:3.7_0": "attheva suttantoti?",
  "kv6.8:3.8_0": "Āmantā.",
  "kv6.8:3.9_0": "Hañci “cakkhuñca paṭicca rūpe ca uppajjati cakkhuviññāṇan”ti—",
  "kv6.8:3.10_0": "attheva suttanto, no ca vata re vattabbe—",
  "kv6.8:3.11_0": "“cakkhuñca paṭicca pathavīdhātuñca uppajjati cakkhuviññāṇan”ti.",
  "kv6.8:4.1_0": "Na vattabbaṁ—",
  "kv6.8:4.2_0": "“pathavīdhātu sanidassanā”ti?",
  "kv6.8:4.3_0": "Āmantā.",
  "kv6.8:4.4_0": "Nanu passati bhūmiṁ pāsāṇaṁ pabbatanti?",
  "kv6.8:4.5_0": "Āmantā.",
  "kv6.8:4.6_0": "Hañci passati bhūmiṁ pāsāṇaṁ pabbataṁ, tena vata re vattabbe—",
  "kv6.8:4.7_0": "“pathavīdhātu sanidassanā”ti …pe….",
  "kv6.8:5.1_0": "Na vattabbaṁ—",
  "kv6.8:5.2_0": "“āpodhātu sanidassanā”ti?",
  "kv6.8:5.3_0": "Āmantā.",
  "kv6.8:5.4_0": "Nanu passati udakanti?",
  "kv6.8:5.5_0": "Āmantā.",
  "kv6.8:5.6_0": "Hañci passati udakaṁ, tena vata re vattabbe—",
  "kv6.8:5.7_0": "“āpodhātu sanidassanāti …pe….",
  "kv6.8:6.1_0": "Na vattabbaṁ—",
  "kv6.8:6.2_0": "tejodhātu sanidassanāti?",
  "kv6.8:6.3_0": "Āmantā.",
  "kv6.8:6.4_0": "Nanu passati aggiṁ jalantanti?",
  "kv6.8:6.5_0": "Āmantā.",
  "kv6.8:6.6_0": "Hañci passati aggiṁ jalantaṁ, tena vata re vattabbe—",
  "kv6.8:6.7_0": "“tejodhātu sanidassanā”ti …pe….",
  "kv6.8:7.1_0": "Na vattabbaṁ—",
  "kv6.8:7.2_0": "“vāyodhātu sanidassanā”ti?",
  "kv6.8:7.3_0": "Āmantā.",
  "kv6.8:7.4_0": "Nanu passati vātena rukkhe sañcāliyamāneti?",
  "kv6.8:7.5_0": "Āmantā.",
  "kv6.8:7.6_0": "Hañci passati vātena rukkhe sañcāliyamāne, tena vata re vattabbe—",
  "kv6.8:7.7_0": "“vāyodhātu sanidassanā”ti …pe….",
  "kv6.8:7.8_0": "Pathavīdhātu sanidassanātiādikathā niṭṭhitā."
}