{
  "patthana15.18:0.1_0": "Paṭṭhānapakaraṇa",
  "patthana15.18:0.2_0": "Dhammānulomapaccanīya (3), Dukatikapaṭṭhānapāḷi (3)",
  "patthana15.18:0.3_0": "15.18. Hetuduka, Atītattika",
  "patthana15.18:1.1_0": "Hetu atīto dhammo nahetussa naatītassa dhammassa ārammaṇapaccayena paccayo.",
  "patthana15.18:1.1_1": "Ārammaṇe nava,",
  "patthana15.18:1.1_2": "adhipatiyā nava,",
  "patthana15.18:1.1_3": "anantare nava …pe…",
  "patthana15.18:1.1_4": "upanissaye nava …pe…",
  "patthana15.18:1.1_5": "vigate nava.",
  "patthana15.18:2.1_0": "Hetu anāgato dhammo nahetussa naanāgatassa dhammassa ārammaṇapaccayena paccayo.",
  "patthana15.18:2.1_1": "Ārammaṇe nava,",
  "patthana15.18:2.1_2": "adhipatiyā upanissaye nava."
}