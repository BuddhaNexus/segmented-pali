{
  "tha-ap536:0.1_0": "Therāpadāna",
  "tha-ap536:0.2_0": "Kaccāyanavagga",
  "tha-ap536:0.3_0": "4. Dabbamallaputtattheraapadāna",
  "tha-ap536:1.1_0": "“Padumuttaro nāma jino,",
  "tha-ap536:1.2_0": "sabbalokavidū muni;",
  "tha-ap536:1.3_0": "Ito satasahassamhi,",
  "tha-ap536:1.4_0": "kappe uppajji cakkhumā.",
  "tha-ap536:2.1_0": "Ovādako viññāpako,",
  "tha-ap536:2.2_0": "tārako sabbapāṇinaṁ;",
  "tha-ap536:2.3_0": "Desanākusalo buddho,",
  "tha-ap536:2.4_0": "tāresi janataṁ bahuṁ.",
  "tha-ap536:3.1_0": "Anukampako kāruṇiko,",
  "tha-ap536:3.2_0": "hitesī sabbapāṇinaṁ;",
  "tha-ap536:3.3_0": "Sampatte titthiye sabbe,",
  "tha-ap536:3.4_0": "pañcasīle patiṭṭhapi.",
  "tha-ap536:4.1_0": "Evaṁ nirākulaṁ āsi,",
  "tha-ap536:4.2_0": "suññataṁ titthiyehi ca;",
  "tha-ap536:4.3_0": "Vicittaṁ arahantehi,",
  "tha-ap536:4.4_0": "vasībhūtehi tādibhi.",
  "tha-ap536:5.1_0": "Ratanānaṭṭhapaññāsaṁ,",
  "tha-ap536:5.2_0": "uggato so mahāmuni;",
  "tha-ap536:5.3_0": "Kañcanagghiyasaṅkāso,",
  "tha-ap536:5.4_0": "bāttiṁsavaralakkhaṇo.",
  "tha-ap536:6.1_0": "Vassasatasahassāni,",
  "tha-ap536:6.2_0": "āyu vijjati tāvade;",
  "tha-ap536:6.3_0": "Tāvatā tiṭṭhamāno so,",
  "tha-ap536:6.4_0": "tāresi janataṁ bahuṁ.",
  "tha-ap536:7.1_0": "Tadāhaṁ haṁsavatiyaṁ,",
  "tha-ap536:7.2_0": "seṭṭhiputto mahāyaso;",
  "tha-ap536:7.3_0": "Upetvā lokapajjotaṁ,",
  "tha-ap536:7.4_0": "assosiṁ dhammadesanaṁ.",
  "tha-ap536:8.1_0": "Senāsanāni bhikkhūnaṁ,",
  "tha-ap536:8.2_0": "paññāpentaṁ sasāvakaṁ;",
  "tha-ap536:8.3_0": "Kittayantassa vacanaṁ,",
  "tha-ap536:8.4_0": "suṇitvā mudito ahaṁ.",
  "tha-ap536:9.1_0": "Adhikāraṁ sasaṅghassa,",
  "tha-ap536:9.2_0": "katvā tassa mahesino;",
  "tha-ap536:9.3_0": "Nipacca sirasā pāde,",
  "tha-ap536:9.4_0": "taṁ ṭhānamabhipatthayiṁ.",
  "tha-ap536:10.1_0": "Tadāha sa mahāvīro,",
  "tha-ap536:10.2_0": "mama kammaṁ pakittayaṁ;",
  "tha-ap536:10.3_0": "‘Yo sasaṅghamabhojesi,",
  "tha-ap536:10.4_0": "sattāhaṁ lokanāyakaṁ.",
  "tha-ap536:11.1_0": "Soyaṁ kamalapattakkho,",
  "tha-ap536:11.2_0": "sīhaṁso kanakattaco;",
  "tha-ap536:11.3_0": "Mama pādamūle nipati,",
  "tha-ap536:11.4_0": "patthayaṁ ṭhānamuttamaṁ.",
  "tha-ap536:12.1_0": "Satasahassito kappe,",
  "tha-ap536:12.2_0": "okkākakulasambhavo;",
  "tha-ap536:12.3_0": "Gotamo nāma gottena,",
  "tha-ap536:12.4_0": "satthā loke bhavissati.",
  "tha-ap536:13.1_0": "Sāvako tassa buddhassa,",
  "tha-ap536:13.2_0": "dabbo nāmena vissuto;",
  "tha-ap536:13.3_0": "Senāsanapaññāpako,",
  "tha-ap536:13.4_0": "aggo hessatiyaṁ tadā’.",
  "tha-ap536:14.1_0": "Tena kammena sukatena,",
  "tha-ap536:14.2_0": "cetanāpaṇidhīhi ca;",
  "tha-ap536:14.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "tha-ap536:14.4_0": "tāvatiṁsamagacchahaṁ.",
  "tha-ap536:15.1_0": "Satānaṁ tīṇikkhattuñca,",
  "tha-ap536:15.2_0": "devarajjamakārayiṁ;",
  "tha-ap536:15.3_0": "Satānaṁ pañcakkhattuñca,",
  "tha-ap536:15.4_0": "cakkavattī ahosahaṁ.",
  "tha-ap536:16.1_0": "Padesarajjaṁ vipulaṁ,",
  "tha-ap536:16.2_0": "gaṇanāto asaṅkhiyaṁ;",
  "tha-ap536:16.3_0": "Sabbattha sukhito āsiṁ,",
  "tha-ap536:16.4_0": "tassa kammassa vāhasā.",
  "tha-ap536:17.1_0": "Ekanavutito kappe,",
  "tha-ap536:17.2_0": "vipassī nāma nāyako;",
  "tha-ap536:17.3_0": "Uppajji cārudassano,",
  "tha-ap536:17.4_0": "sabbadhammavipassako.",
  "tha-ap536:18.1_0": "Duṭṭhacitto upavadiṁ,",
  "tha-ap536:18.2_0": "sāvakaṁ tassa tādino;",
  "tha-ap536:18.3_0": "Sabbāsavaparikkhīṇaṁ,",
  "tha-ap536:18.4_0": "suddhoti ca vijāniya.",
  "tha-ap536:19.1_0": "Tasseva naravīrassa,",
  "tha-ap536:19.2_0": "sāvakānaṁ mahesinaṁ;",
  "tha-ap536:19.3_0": "Salākañca gahetvāna,",
  "tha-ap536:19.4_0": "khīrodanamadāsahaṁ.",
  "tha-ap536:20.1_0": "Imamhi bhaddake kappe,",
  "tha-ap536:20.2_0": "brahmabandhu mahāyaso;",
  "tha-ap536:20.3_0": "Kassapo nāma gottena,",
  "tha-ap536:20.4_0": "uppajji vadataṁ varo.",
  "tha-ap536:21.1_0": "Sāsanaṁ jotayitvāna,",
  "tha-ap536:21.2_0": "abhibhuyya kutitthiye;",
  "tha-ap536:21.3_0": "Vineyye vinayitvāva,",
  "tha-ap536:21.4_0": "nibbuto so sasāvako.",
  "tha-ap536:22.1_0": "Sasisse nibbute nāthe,",
  "tha-ap536:22.2_0": "atthamentamhi sāsane;",
  "tha-ap536:22.3_0": "Devā kandiṁsu saṁviggā,",
  "tha-ap536:22.4_0": "muttakesā rudammukhā.",
  "tha-ap536:23.1_0": "‘Nibbāyissati dhammakkho,",
  "tha-ap536:23.2_0": "na passissāma subbate;",
  "tha-ap536:23.3_0": "Na suṇissāma saddhammaṁ,",
  "tha-ap536:23.4_0": "aho no appapuññatā’.",
  "tha-ap536:24.1_0": "Tadāyaṁ pathavī sabbā,",
  "tha-ap536:24.2_0": "acalā sā calācalā;",
  "tha-ap536:24.3_0": "Sāgaro ca sasokova,",
  "tha-ap536:24.4_0": "vinadī karuṇaṁ giraṁ.",
  "tha-ap536:25.1_0": "Catuddisā dundubhiyo,",
  "tha-ap536:25.2_0": "nādayiṁsu amānusā;",
  "tha-ap536:25.3_0": "Samantato asaniyo,",
  "tha-ap536:25.4_0": "phaliṁsu ca bhayāvahā.",
  "tha-ap536:26.1_0": "Ukkā patiṁsu nabhasā,",
  "tha-ap536:26.2_0": "dhūmaketu ca dissati;",
  "tha-ap536:26.3_0": "Sadhūmā jālavaṭṭā ca,",
  "tha-ap536:26.4_0": "raviṁsu karuṇaṁ migā.",
  "tha-ap536:27.1_0": "Uppāde dāruṇe disvā,",
  "tha-ap536:27.2_0": "sāsanatthaṅgasūcake;",
  "tha-ap536:27.3_0": "Saṁviggā bhikkhavo satta,",
  "tha-ap536:27.4_0": "cintayimha mayaṁ tadā.",
  "tha-ap536:28.1_0": "‘Sāsanena vināmhākaṁ,",
  "tha-ap536:28.2_0": "jīvitena alaṁ mayaṁ;",
  "tha-ap536:28.3_0": "Pavisitvā mahāraññaṁ,",
  "tha-ap536:28.4_0": "yuñjāma jinasāsanaṁ’.",
  "tha-ap536:29.1_0": "Addasamha tadāraññe,",
  "tha-ap536:29.2_0": "ubbiddhaṁ selamuttamaṁ;",
  "tha-ap536:29.3_0": "Nisseṇiyā tamāruyha,",
  "tha-ap536:29.4_0": "nisseṇiṁ pātayimhase.",
  "tha-ap536:30.1_0": "Tadā ovadi no thero,",
  "tha-ap536:30.2_0": "‘buddhuppādo sudullabho;",
  "tha-ap536:30.3_0": "Saddhātidullabhā laddhā,",
  "tha-ap536:30.4_0": "thokaṁ sesañca sāsanaṁ.",
  "tha-ap536:31.1_0": "Nipatanti khaṇātītā,",
  "tha-ap536:31.2_0": "anante dukkhasāgare;",
  "tha-ap536:31.3_0": "Tasmā payogo kattabbo,",
  "tha-ap536:31.4_0": "yāva ṭhāti mune mataṁ’.",
  "tha-ap536:32.1_0": "Arahā āsi so thero,",
  "tha-ap536:32.2_0": "anāgāmī tadānugo;",
  "tha-ap536:32.3_0": "Susīlā itare yuttā,",
  "tha-ap536:32.4_0": "devalokaṁ agamhase.",
  "tha-ap536:33.1_0": "Nibbuto tiṇṇasaṁsāro,",
  "tha-ap536:33.2_0": "suddhāvāse ca ekako;",
  "tha-ap536:33.3_0": "Ahañca pakkusāti ca,",
  "tha-ap536:33.4_0": "sabhiyo bāhiyo tathā.",
  "tha-ap536:34.1_0": "Kumārakassapo ceva,",
  "tha-ap536:34.2_0": "tattha tatthūpagā mayaṁ;",
  "tha-ap536:34.3_0": "Saṁsārabandhanā muttā,",
  "tha-ap536:34.4_0": "gotamenānukampitā.",
  "tha-ap536:35.1_0": "Mallesu kusinārāyaṁ,",
  "tha-ap536:35.2_0": "jāto gabbheva me sato;",
  "tha-ap536:35.3_0": "Mātā matā citāruḷhā,",
  "tha-ap536:35.4_0": "tato nippatito ahaṁ.",
  "tha-ap536:36.1_0": "Patito dabbapuñjamhi,",
  "tha-ap536:36.2_0": "tato dabboti vissuto;",
  "tha-ap536:36.3_0": "Brahmacārībalenāhaṁ,",
  "tha-ap536:36.4_0": "vimutto sattavassiko.",
  "tha-ap536:37.1_0": "Khīrodanabalenāhaṁ,",
  "tha-ap536:37.2_0": "pañcahaṅgehupāgato;",
  "tha-ap536:37.3_0": "Khīṇāsavopavādena,",
  "tha-ap536:37.4_0": "pāpehi bahucodito.",
  "tha-ap536:38.1_0": "Ubho puññañca pāpañca,",
  "tha-ap536:38.2_0": "vītivattomhi dānihaṁ;",
  "tha-ap536:38.3_0": "Patvāna paramaṁ santiṁ,",
  "tha-ap536:38.4_0": "viharāmi anāsavo.",
  "tha-ap536:39.1_0": "Senāsanaṁ paññāpayiṁ,",
  "tha-ap536:39.2_0": "hāsayitvāna subbate;",
  "tha-ap536:39.3_0": "Jino tasmiṁ guṇe tuṭṭho,",
  "tha-ap536:39.4_0": "etadagge ṭhapesi maṁ.",
  "tha-ap536:40.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap536:40.2_0": "…pe…",
  "tha-ap536:40.3_0": "viharāmi anāsavo.",
  "tha-ap536:41.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap536:41.2_0": "…pe…",
  "tha-ap536:41.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap536:42.1_0": "Paṭisambhidā catasso,",
  "tha-ap536:42.2_0": "…pe…",
  "tha-ap536:42.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap536:43.1_0": "Itthaṁ sudaṁ āyasmā dabbamallaputto thero imā gāthāyo abhāsitthāti.",
  "tha-ap536:44.1_0": "Dabbamallaputtattherassāpadānaṁ catutthaṁ."
}