{
  "mil3.3.1:0.1_0": "Milindapañha",
  "mil3.3.1:0.2_0": "Vicāravagga",
  "mil3.3.1:0.3_0": "1. Addhānamūlapañha",
  "mil3.3.1:1.1_0": "Rājā āha—",
  "mil3.3.1:1.2_0": "“bhante nāgasena,",
  "mil3.3.1:1.2_1": "atītassa addhānassa kiṁ mūlaṁ,",
  "mil3.3.1:1.2_2": "anāgatassa addhānassa kiṁ mūlaṁ,",
  "mil3.3.1:1.2_3": "paccuppannassa addhānassa kiṁ mūlan”ti?",
  "mil3.3.1:1.3_0": "“Atītassa ca, mahārāja,",
  "mil3.3.1:1.3_1": "addhānassa anāgatassa ca addhānassa paccuppannassa ca addhānassa avijjā mūlaṁ.",
  "mil3.3.1:1.4_0": "Avijjāpaccayā saṅkhārā,",
  "mil3.3.1:1.4_1": "saṅkhārapaccayā viññāṇaṁ,",
  "mil3.3.1:1.4_2": "viññāṇapaccayā nāmarūpaṁ,",
  "mil3.3.1:1.4_3": "nāmarūpapaccayā saḷāyatanaṁ,",
  "mil3.3.1:1.4_4": "saḷāyatanapaccayā phasso,",
  "mil3.3.1:1.4_5": "phassapaccayā vedanā,",
  "mil3.3.1:1.4_6": "vedanāpaccayā taṇhā,",
  "mil3.3.1:1.4_7": "taṇhāpaccayā upādānaṁ,",
  "mil3.3.1:1.4_8": "upādānapaccayā bhavo,",
  "mil3.3.1:1.4_9": "bhavapaccayā jāti,",
  "mil3.3.1:1.4_10": "jātipaccayā jarāmaraṇaṁ sokaparidevadukkhadomanassupāyāsā sambhavanti.",
  "mil3.3.1:1.5_0": "Evametassa kevalassa dukkhakkhandhassa addhānassa purimā koṭi na paññāyatī”ti.",
  "mil3.3.1:2.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.3.1:3.1_0": "Addhānamūlapañho paṭhamo."
}