{
  "mil4.4:0.1_0": "Milindapañha",
  "mil4.4:0.2_0": "Meṇḍakapañhārambhakathā",
  "mil4.4:0.3_0": "Upāsakaguṇavagga",
  "mil4.4:0.4_0": "4. Aṭṭhapaññāpaṭilābhakāraṇa",
  "mil4.4:1.1_0": "“Bhante nāgasena,",
  "mil4.4:1.1_1": "aṭṭhahi kāraṇehi buddhi pariṇamati paripākaṁ gacchati.",
  "mil4.4:1.2_0": "Katamehi aṭṭhahi?",
  "mil4.4:1.3_0": "Vayapariṇāmena buddhi pariṇamati paripākaṁ gacchati,",
  "mil4.4:1.3_1": "yasapariṇāmena buddhi pariṇamati paripākaṁ gacchati,",
  "mil4.4:1.3_2": "paripucchāya buddhi pariṇamati paripākaṁ gacchati,",
  "mil4.4:1.3_3": "titthasaṁvāsena buddhi pariṇamati paripākaṁ gacchati,",
  "mil4.4:1.3_4": "yoniso manasikārena buddhi pariṇamati paripākaṁ gacchati,",
  "mil4.4:1.3_5": "sākacchāya buddhi pariṇamati paripākaṁ gacchati,",
  "mil4.4:1.3_6": "snehūpasevanena buddhi pariṇamati paripākaṁ gacchati,",
  "mil4.4:1.3_7": "patirūpadesavāsena buddhi pariṇamati paripākaṁ gacchati.",
  "mil4.4:1.4_0": "Bhavatīha—",
  "mil4.4:2.1_0": "‘Vayena yasapucchāhi,",
  "mil4.4:2.2_0": "titthavāsena yoniso;",
  "mil4.4:2.3_0": "Sākacchā snehasaṁsevā,",
  "mil4.4:2.4_0": "patirūpavasena ca.",
  "mil4.4:3.1_0": "Etāni aṭṭha ṭhānāni,",
  "mil4.4:3.2_0": "buddhivisadakāraṇā;",
  "mil4.4:3.3_0": "Yesaṁ etāni sambhonti,",
  "mil4.4:3.4_0": "tesaṁ buddhi pabhijjatī’”ti.",
  "mil4.4:4.1_0": "Aṭṭha paññāpaṭilābhakāraṇāni."
}