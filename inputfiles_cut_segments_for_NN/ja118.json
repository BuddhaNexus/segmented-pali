{
  "ja118:0.1_0": "Jātaka",
  "ja118:0.2_0": "Ekakanipāta",
  "ja118:0.3_0": "Haṁcivagga",
  "ja118:0.4_0": "8. Vaṭṭakajātaka",
  "ja118:1.1_0": "“Nācintayanto puriso,",
  "ja118:1.2_0": "visesamadhigacchati;",
  "ja118:1.3_0": "Cintitassa phalaṁ passa,",
  "ja118:1.4_0": "muttosmi vadhabandhanā”ti.",
  "ja118:2.1_0": "Vaṭṭakajātakaṁ aṭṭhamaṁ."
}