{
  "mil7.6.6:0.1_0": "Milindapañha",
  "mil7.6.6:0.2_0": "Opammakathāpañha",
  "mil7.6.6:0.3_0": "Sīhavagga",
  "mil7.6.6:0.4_0": "6. Satapattaṅgapañha",
  "mil7.6.6:1.1_0": "“Bhante nāgasena,",
  "mil7.6.6:1.1_1": "‘satapattassa ekaṁ aṅgaṁ gahetabban’ti yaṁ vadesi,",
  "mil7.6.6:1.1_2": "katamaṁ taṁ ekaṁ aṅgaṁ gahetabban”ti?",
  "mil7.6.6:1.2_0": "“Yathā, mahārāja, satapatto ravitvā paresaṁ khemaṁ vā bhayaṁ vā ācikkhati;",
  "mil7.6.6:1.3_0": "evameva kho, mahārāja,",
  "mil7.6.6:1.3_1": "yoginā yogāvacarena paresaṁ dhammaṁ desayamānena vinipātaṁ bhayato dassayitabbaṁ,",
  "mil7.6.6:1.3_2": "nibbānaṁ khemato dassayitabbaṁ.",
  "mil7.6.6:1.4_0": "Idaṁ, mahārāja, satapattassa ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.6.6:1.5_0": "Bhāsitampetaṁ, mahārāja, therena piṇḍolabhāradvājena—",
  "mil7.6.6:2.1_0": "‘Niraye bhayasantāsaṁ,",
  "mil7.6.6:2.2_0": "nibbāne vipulaṁ sukhaṁ;",
  "mil7.6.6:2.3_0": "Ubhayānetānatthāni,",
  "mil7.6.6:2.4_0": "dassetabbāni yoginā’”ti.",
  "mil7.6.6:3.1_0": "Satapattaṅgapañho chaṭṭho."
}