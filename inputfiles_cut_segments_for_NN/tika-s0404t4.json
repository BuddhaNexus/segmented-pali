{
  "tika-s0404t4:0_0": "5. Uposathavaggo",
  "tika-s0404t4:1_0": "1-8. Saṁkhittūposathasuttādivaṇṇanā",
  "tika-s0404t4:2_0": "41-48.",
  "tika-s0404t4:2_1": "Pañcamassa paṭhamādīsu natthi vattabbaṁ.",
  "tika-s0404t4:2_2": "Chaṭṭhe (saṁ. ni. ṭī.",
  "tika-s0404t4:2_3": "1.1.165) pañca aṅgāni etassāti pañcaṅgaṁ,",
  "tika-s0404t4:2_4": "pañcaṅgameva pañcaṅgikaṁ,",
  "tika-s0404t4:2_5": "tassa pañcaṅgikassa.",
  "tika-s0404t4:2_6": "Mahatī daddarī vīṇāvisesopi ātatamevāti ‘‘cammapariyonaddhesū’’ti visesanaṁ kataṁ.",
  "tika-s0404t4:2_7": "Ekatalatūriyaṁ kumbhathunadaddarikādi.",
  "tika-s0404t4:2_8": "Ubhayatalaṁ bherimudiṅgādi.",
  "tika-s0404t4:2_9": "Cammapariyonaddhaṁ hutvā vinibaddhaṁ ātatavitataṁ.",
  "tika-s0404t4:2_10": "Sabbaso pariyonaddhaṁ nāma caturassaambaṇaṁ paṇavādi ca.",
  "tika-s0404t4:2_11": "Gomukhīādīnampi ettheva saṅgaho daṭṭhabbo.",
  "tika-s0404t4:2_12": "Vaṁsādīti ādi-saddena saṅkhādīnaṁ saṅgaho.",
  "tika-s0404t4:2_13": "Sammādīti sammatāḷakaṁsatāḷasilāsalākatāḷādi.",
  "tika-s0404t4:2_14": "Tattha sammatāḷaṁ nāma dantamayatāḷaṁ.",
  "tika-s0404t4:2_15": "Kaṁsatāḷaṁ lohamayaṁ.",
  "tika-s0404t4:2_16": "Silāmayaṁ ayopattena ca vādanatāḷaṁ silāsalākatāḷaṁ.",
  "tika-s0404t4:2_17": "Sumucchitassāti suṭṭhu paṭiyattassa.",
  "tika-s0404t4:2_18": "Pamāṇeti nātidaḷhanātisithilasaṅkhāte majjhime mucchanāpamāṇe.",
  "tika-s0404t4:2_19": "Chekoti paṭu paṭṭho.",
  "tika-s0404t4:2_20": "So cassa paṭubhāvo manoharoti āha ‘‘sundaro’’ti.",
  "tika-s0404t4:2_21": "Rañjetunti rāgaṁ uppādetuṁ.",
  "tika-s0404t4:2_22": "Khamatevāti rocateva.",
  "tika-s0404t4:2_23": "Na nibbindatīti na tajjeti,",
  "tika-s0404t4:2_24": "sotasukhabhāvato piyāyitabbova hoti.",
  "tika-s0404t4:3_0": "Bhattāraṁ nātimaññatīti sāmikaṁ muñcitvā aññaṁ manasāpi na pattheti.",
  "tika-s0404t4:3_1": "Uṭṭhāhikāti uṭṭhānavīriyasampannā.",
  "tika-s0404t4:3_2": "Analasāti nikkosajjā.",
  "tika-s0404t4:3_3": "Saṅgahitaparijjanāti sammānanādīhi ceva chaṇādīsu pesetabba-piyabhaṇḍādipaṇṇākārapesanādīhi ca saṅgahitaparijanā.",
  "tika-s0404t4:3_4": "Idha parijano nāma sāmikassa ceva attano ca ñātijano.",
  "tika-s0404t4:3_5": "Sambhatanti kasivaṇijjādīni katvā ābhatadhanaṁ.",
  "tika-s0404t4:3_6": "Sattamaṭṭhamāni uttānatthāni.",
  "tika-s0404t4:4_0": "Saṁkhittūposathasuttādivaṇṇanā niṭṭhitā.",
  "tika-s0404t4:5_0": "9-10. Paṭhamaidhalokikasuttādivaṇṇanā",
  "tika-s0404t4:6_0": "49-50.",
  "tika-s0404t4:6_1": "Navame idhalokavijayāyāti idhalokavijinanatthāya abhibhavatthāya.",
  "tika-s0404t4:6_2": "Yo hi diṭṭhadhammikaṁ anatthaṁ parivajjanavasena abhibhavati,",
  "tika-s0404t4:6_3": "tato eva tadatthaṁ sampādeti,",
  "tika-s0404t4:6_4": "so idhalokavijayāya paṭipanno nāma hoti paccatthikaniggaṇhanato sadatthasampādanato ca.",
  "tika-s0404t4:6_5": "Tenāha ‘‘ayaṁsa loko āraddho hotī’’ti.",
  "tika-s0404t4:6_6": "(Pasaṁsāvahato tayidaṁ pasaṁsāvahanaṁ kittisaddena idhaloke saddānaṁ cittatosanaviddheyyabhāvāpādanena",
  "tika-s0404t4:6_7": "ca hotīti daṭṭhabbaṁ.) Susaṁvihitakammantoti yāgubhattapacanakālādīni anatikkamitvā tassa tassa sādhukaṁ",
  "tika-s0404t4:6_8": "karaṇena suṭṭhu saṁvihitakammanto.",
  "tika-s0404t4:6_9": "Paralokavijayāyāti paralokassa vijinanatthāya abhibhavatthāya.",
  "tika-s0404t4:6_10": "Yo hi samparāyikaṁ anatthaṁ parivajjanavasena abhibhavati,",
  "tika-s0404t4:6_11": "tato eva tadatthaṁ sampādeti,",
  "tika-s0404t4:6_12": "so paralokavijayāya paṭipanno nāma hoti.",
  "tika-s0404t4:6_13": "Sesaṁ sabbattha uttānameva.",
  "tika-s0404t4:7_0": "Paṭhamaidhalokikasuttādivaṇṇanā niṭṭhitā.",
  "tika-s0404t4:8_0": "Uposathavaggavaṇṇanā niṭṭhitā.",
  "tika-s0404t4:9_0": "Paṭhamapaṇṇāsakaṁ niṭṭhitaṁ.",
  "tika-s0404t4:10_0": "2. Dutiyapaṇṇāsakaṁ"
}