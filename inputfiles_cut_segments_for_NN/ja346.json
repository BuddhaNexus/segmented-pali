{
  "ja346:0.1_0": "Jātaka",
  "ja346:0.2_0": "Catukkanipāta",
  "ja346:0.3_0": "Cūḷakuṇālavagga",
  "ja346:0.4_0": "6. Kesavajātaka",
  "ja346:1.1_0": "“Manussindaṁ jahitvāna,",
  "ja346:1.2_0": "sabbakāmasamiddhinaṁ;",
  "ja346:1.3_0": "Kathaṁ nu bhagavā kesī,",
  "ja346:1.4_0": "kappassa ramati assame”.",
  "ja346:2.1_0": "“Sādūni ramaṇīyāni,",
  "ja346:2.2_0": "santi vakkhā manoramā;",
  "ja346:2.3_0": "Subhāsitāni kappassa,",
  "ja346:2.4_0": "nārada ramayanti maṁ”.",
  "ja346:3.1_0": "“Sālīnaṁ odanaṁ bhuñje,",
  "ja346:3.2_0": "suciṁ maṁsūpasecanaṁ;",
  "ja346:3.3_0": "Kathaṁ sāmākanīvāraṁ,",
  "ja346:3.4_0": "aloṇaṁ chādayanti taṁ”.",
  "ja346:4.1_0": "“Sāduṁ vā yadi vāsāduṁ,",
  "ja346:4.2_0": "appaṁ vā yadi vā bahuṁ;",
  "ja346:4.3_0": "Vissattho yattha bhuñjeyya,",
  "ja346:4.4_0": "vissāsaparamā rasā”ti.",
  "ja346:5.1_0": "Kesavajātakaṁ chaṭṭhaṁ."
}