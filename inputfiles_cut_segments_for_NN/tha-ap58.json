{
  "tha-ap58:0.1_0": "Therāpadāna",
  "tha-ap58:0.2_0": "Bījanivagga",
  "tha-ap58:0.3_0": "6. Saparivārāsanattheraapadāna",
  "tha-ap58:1.1_0": "“Padumuttarabuddhassa,",
  "tha-ap58:1.2_0": "piṇḍapātaṁ adāsahaṁ;",
  "tha-ap58:1.3_0": "Gantvā kiliṭṭhakaṁ ṭhānaṁ,",
  "tha-ap58:1.4_0": "mallikāhi parikkhitaṁ.",
  "tha-ap58:2.1_0": "Tamhāsanamhi āsīno,",
  "tha-ap58:2.2_0": "buddho lokagganāyako;",
  "tha-ap58:2.3_0": "Akittayi piṇḍapātaṁ,",
  "tha-ap58:2.4_0": "ujubhūto samāhito.",
  "tha-ap58:3.1_0": "Yathāpi bhaddake khette,",
  "tha-ap58:3.2_0": "bījaṁ appampi ropitaṁ;",
  "tha-ap58:3.3_0": "Sammā dhāraṁ pavecchante,",
  "tha-ap58:3.4_0": "phalaṁ toseti kassakaṁ.",
  "tha-ap58:4.1_0": "Tathevāyaṁ piṇḍapāto,",
  "tha-ap58:4.2_0": "sukhette ropito tayā;",
  "tha-ap58:4.3_0": "Bhave nibbattamānamhi,",
  "tha-ap58:4.4_0": "phalaṁ te tosayissati.",
  "tha-ap58:5.1_0": "Idaṁ vatvāna sambuddho,",
  "tha-ap58:5.2_0": "jalajuttamanāmako;",
  "tha-ap58:5.3_0": "Piṇḍapātaṁ gahetvāna,",
  "tha-ap58:5.4_0": "pakkāmi uttarāmukho.",
  "tha-ap58:6.1_0": "Saṁvuto pātimokkhasmiṁ,",
  "tha-ap58:6.2_0": "indriyesu ca pañcasu;",
  "tha-ap58:6.3_0": "Pavivekamanuyutto,",
  "tha-ap58:6.4_0": "viharāmi anāsavo.",
  "tha-ap58:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap58:7.2_0": "…pe…",
  "tha-ap58:7.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap58:8.1_0": "Itthaṁ sudaṁ āyasmā saparivārāsano thero imā gāthāyo abhāsitthāti.",
  "tha-ap58:9.1_0": "Saparivārāsanattherassāpadānaṁ chaṭṭhaṁ."
}