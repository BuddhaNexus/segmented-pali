{
  "mil6.2.7:0.1_0": "Milindapañha",
  "mil6.2.7:0.2_0": "Anumānapañha",
  "mil6.2.7:0.3_0": "Nippapañcavagga",
  "mil6.2.7:0.4_0": "7. Yakkhapañha",
  "mil6.2.7:1.1_0": "“Bhante nāgasena, atthi loke yakkhā nāmā”ti?",
  "mil6.2.7:1.2_0": "“Āma, mahārāja, atthi loke yakkhā nāmā”ti.",
  "mil6.2.7:1.3_0": "“Cavanti pana te, bhante, yakkhā tamhā yoniyā”ti?",
  "mil6.2.7:1.4_0": "“Āma, mahārāja, cavanti te yakkhā tamhā yoniyā”ti.",
  "mil6.2.7:1.5_0": "“Kissa pana,",
  "mil6.2.7:1.5_1": "bhante nāgasena,",
  "mil6.2.7:1.5_2": "tesaṁ matānaṁ yakkhānaṁ sarīraṁ na dissati,",
  "mil6.2.7:1.5_3": "kuṇapagandhopi na vāyatī”ti?",
  "mil6.2.7:1.6_0": "“Dissati, mahārāja,",
  "mil6.2.7:1.6_1": "matānaṁ yakkhānaṁ sarīraṁ,",
  "mil6.2.7:1.6_2": "kuṇapagandhopi tesaṁ vāyati, matānaṁ, mahārāja,",
  "mil6.2.7:1.6_3": "yakkhānaṁ sarīraṁ kīṭavaṇṇena vā dissati,",
  "mil6.2.7:1.6_4": "kimivaṇṇena vā dissati,",
  "mil6.2.7:1.6_5": "kipillikavaṇṇena vā dissati,",
  "mil6.2.7:1.6_6": "paṭaṅgavaṇṇena vā dissati,",
  "mil6.2.7:1.6_7": "ahivaṇṇena vā dissati,",
  "mil6.2.7:1.6_8": "vicchikavaṇṇena vā dissati,",
  "mil6.2.7:1.6_9": "satapadivaṇṇena vā dissati,",
  "mil6.2.7:1.6_10": "dijavaṇṇena vā dissati,",
  "mil6.2.7:1.6_11": "migavaṇṇena vā dissatī”ti.",
  "mil6.2.7:1.7_0": "“Ko hi, bhante nāgasena, añño idaṁ pañhaṁ puṭṭho visajjeyya aññatra tavādisena buddhimatā”ti.",
  "mil6.2.7:2.1_0": "Yakkhapañho sattamo."
}