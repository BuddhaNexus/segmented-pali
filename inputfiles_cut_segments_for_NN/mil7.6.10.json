{
  "mil7.6.10:0.1_0": "Milindapañha",
  "mil7.6.10:0.2_0": "Opammakathāpañha",
  "mil7.6.10:0.3_0": "Sīhavagga",
  "mil7.6.10:0.4_0": "10. Ajagaraṅgapañha",
  "mil7.6.10:1.1_0": "“Bhante nāgasena,",
  "mil7.6.10:1.1_1": "‘ajagarassa ekaṁ aṅgaṁ gahetabban’ti yaṁ vadesi,",
  "mil7.6.10:1.1_2": "katamaṁ taṁ ekaṁ aṅgaṁ gahetabban”ti?",
  "mil7.6.10:1.2_0": "“Yathā, mahārāja,",
  "mil7.6.10:1.2_1": "ajagaro mahatimahākāyo bahūpi divase ūnūdaro dīnataro kucchipūraṁ āhāraṁ na labhati,",
  "mil7.6.10:1.2_2": "aparipuṇṇo yeva yāvadeva sarīrayāpanamattakena yāpeti;",
  "mil7.6.10:1.3_0": "evameva kho, mahārāja,",
  "mil7.6.10:1.3_1": "yogino yogāvacarassa bhikkhācariyappasutassa parapiṇḍamupagatassa paradinnappāṭikaṅkhissa sayaṅgāhappaṭiviratassa dullabhaṁ udaraparipūraṁ āhāraṁ,",
  "mil7.6.10:1.3_2": "api ca atthavasikena kulaputtena cattāro pañca ālope abhuñjitvā avasesaṁ udakena paripūretabbaṁ.",
  "mil7.6.10:1.4_0": "Idaṁ, mahārāja, ajagarassa ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.6.10:1.5_0": "Bhāsitampetaṁ, mahārāja, therena sāriputtena dhammasenāpatinā—",
  "mil7.6.10:2.1_0": "‘Allaṁ sukkhaṁ vā bhuñjanto,",
  "mil7.6.10:2.2_0": "Na bāḷhaṁ suhito siyā;",
  "mil7.6.10:2.3_0": "Ūnūdaro mitāhāro,",
  "mil7.6.10:2.4_0": "Sato bhikkhu paribbaje.",
  "mil7.6.10:3.1_0": "Cattāro pañca ālope,",
  "mil7.6.10:3.2_0": "abhutvā udakaṁ pive;",
  "mil7.6.10:3.3_0": "Alaṁ phāsu vihārāya,",
  "mil7.6.10:3.4_0": "pahitattassa bhikkhuno’”ti.",
  "mil7.6.10:4.1_0": "Ajagaraṅgapañho dasamo.",
  "mil7.6.10:5.1_0": "Sīhavaggo pañcamo.",
  "mil7.6.10:6.1_0": "Tassuddānaṁ",
  "mil7.6.10:7.1_0": "Kesarī cakkavāko ca,",
  "mil7.6.10:7.2_0": "peṇāhi gharakapotako;",
  "mil7.6.10:7.3_0": "Ulūko satapatto ca,",
  "mil7.6.10:7.4_0": "vagguli ca jalūpikā;",
  "mil7.6.10:7.5_0": "Sappo ajagaro ceva,",
  "mil7.6.10:7.6_0": "vaggo tena pavuccatīti."
}