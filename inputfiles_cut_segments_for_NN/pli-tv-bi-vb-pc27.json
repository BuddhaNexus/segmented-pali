{
  "pli-tv-bi-vb-pc27:0.1_0": "Theravāda Vinaya",
  "pli-tv-bi-vb-pc27:0.2_0": "Bhikkhunivibhaṅga",
  "pli-tv-bi-vb-pc27:0.3_0": "Pācittiyakaṇḍa",
  "pli-tv-bi-vb-pc27:0.4_0": "Naggavagga",
  "pli-tv-bi-vb-pc27:0.5_0": "27. Paṭibāhanasikkhāpada",
  "pli-tv-bi-vb-pc27:1.1_0": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bi-vb-pc27:1.2_0": "Tena kho pana samayena bhikkhunisaṅghassa akālacīvaraṁ uppannaṁ hoti.",
  "pli-tv-bi-vb-pc27:1.3_0": "Atha kho bhikkhunisaṅgho taṁ cīvaraṁ bhājetukāmo sannipati.",
  "pli-tv-bi-vb-pc27:1.4_0": "Tena kho pana samayena thullanandāya bhikkhuniyā antevāsiniyo bhikkhuniyo pakkantā honti.",
  "pli-tv-bi-vb-pc27:1.5_0": "Thullanandā bhikkhunī tā bhikkhuniyo etadavoca—",
  "pli-tv-bi-vb-pc27:1.6_0": "“ayye, bhikkhuniyo pakkantā,",
  "pli-tv-bi-vb-pc27:1.6_1": "na tāva cīvaraṁ bhājīyissatī”ti.",
  "pli-tv-bi-vb-pc27:1.7_0": "Cīvaravibhaṅgaṁ paṭibāhi.",
  "pli-tv-bi-vb-pc27:1.8_0": "Bhikkhuniyo na tāva cīvaraṁ bhājīyissatīti pakkamiṁsu.",
  "pli-tv-bi-vb-pc27:1.9_0": "Thullanandā bhikkhunī antevāsinīsu bhikkhunīsu āgatāsu taṁ cīvaraṁ bhājāpesi.",
  "pli-tv-bi-vb-pc27:1.10_0": "Yā tā bhikkhuniyo appicchā …pe… tā ujjhāyanti khiyyanti vipācenti—",
  "pli-tv-bi-vb-pc27:1.11_0": "“kathañhi nāma ayyā thullanandā dhammikaṁ cīvaravibhaṅgaṁ paṭibāhissatī”ti …pe…",
  "pli-tv-bi-vb-pc27:1.12_0": "“saccaṁ kira, bhikkhave,",
  "pli-tv-bi-vb-pc27:1.12_1": "thullanandā bhikkhunī dhammikaṁ cīvaravibhaṅgaṁ paṭibāhatī”ti?",
  "pli-tv-bi-vb-pc27:1.13_0": "“Saccaṁ, bhagavā”ti.",
  "pli-tv-bi-vb-pc27:1.14_0": "Vigarahi buddho bhagavā …pe…",
  "pli-tv-bi-vb-pc27:1.15_0": "kathañhi nāma, bhikkhave,",
  "pli-tv-bi-vb-pc27:1.15_1": "thullanandā bhikkhunī dhammikaṁ cīvaravibhaṅgaṁ paṭibāhissati.",
  "pli-tv-bi-vb-pc27:1.16_0": "Netaṁ, bhikkhave, appasannānaṁ vā pasādāya …pe…",
  "pli-tv-bi-vb-pc27:1.17_0": "evañca pana, bhikkhave,",
  "pli-tv-bi-vb-pc27:1.17_1": "bhikkhuniyo imaṁ sikkhāpadaṁ uddisantu—",
  "pli-tv-bi-vb-pc27:1.18.1_0": "“Yā pana bhikkhunī dhammikaṁ cīvaravibhaṅgaṁ paṭibāheyya, pācittiyan”ti.",
  "pli-tv-bi-vb-pc27:2.1.1_0": "Yā panāti",
  "pli-tv-bi-vb-pc27:2.1.2_0": "yā yādisā …pe…",
  "pli-tv-bi-vb-pc27:2.1.3_0": "bhikkhunīti",
  "pli-tv-bi-vb-pc27:2.1.4_0": "…pe… ayaṁ imasmiṁ atthe adhippetā bhikkhunīti.",
  "pli-tv-bi-vb-pc27:2.1.5_0": "Dhammiko nāma cīvaravibhaṅgo",
  "pli-tv-bi-vb-pc27:2.1.6_0": "samaggo bhikkhunisaṅgho sannipatitvā bhājeti.",
  "pli-tv-bi-vb-pc27:2.1.7_0": "Paṭibāheyyāti",
  "pli-tv-bi-vb-pc27:2.1.8_0": "kathaṁ imaṁ cīvaraṁ na bhājeyyāti paṭibāhati, āpatti pācittiyassa.",
  "pli-tv-bi-vb-pc27:2.2.1_0": "Dhammike dhammikasaññā paṭibāhati, āpatti pācittiyassa.",
  "pli-tv-bi-vb-pc27:2.2.2_0": "Dhammike vematikā paṭibāhati, āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc27:2.2.3_0": "Dhammike adhammikasaññā paṭibāhati, anāpatti.",
  "pli-tv-bi-vb-pc27:2.2.4_0": "Adhammike dhammikasaññā, āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc27:2.2.5_0": "Adhammike vematikā, āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc27:2.2.6_0": "Adhammike adhammikasaññā, anāpatti.",
  "pli-tv-bi-vb-pc27:2.3.1_0": "Anāpatti—",
  "pli-tv-bi-vb-pc27:2.3.2_0": "ānisaṁsaṁ dassetvā paṭibāhati,",
  "pli-tv-bi-vb-pc27:2.3.3_0": "ummattikāya,",
  "pli-tv-bi-vb-pc27:2.3.4_0": "ādikammikāyāti.",
  "pli-tv-bi-vb-pc27:2.3.5_0": "Sattamasikkhāpadaṁ niṭṭhitaṁ."
}