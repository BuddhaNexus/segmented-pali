{
  "pp1.1:0.1_0": "Puggalapaññatti",
  "pp1.1:0.2_0": "Mātikā",
  "pp1.1:0.3_0": "1. Ekakauddesa",
  "pp1.1:1.1_0": "Namo tassa Bhagavato Arahato Sammāsambuddhassa.",
  "pp1.1:2.1_0": "Cha paññattiyo—",
  "pp1.1:2.2_0": "khandhapaññatti, āyatanapaññatti, dhātupaññatti, saccapaññatti, indriyapaññatti, puggalapaññattīti.",
  "pp1.1:3.1_0": "Kittāvatā khandhānaṁ khandhapaññatti?",
  "pp1.1:3.2_0": "Yāvatā pañcakkhandhā—",
  "pp1.1:3.3_0": "rūpakkhandho, vedanākkhandho, saññākkhandho, saṅkhārakkhandho, viññāṇakkhandho;",
  "pp1.1:3.4_0": "ettāvatā khandhānaṁ khandhapaññatti.",
  "pp1.1:4.1_0": "Kittāvatā āyatanānaṁ āyatanapaññatti?",
  "pp1.1:4.2_0": "Yāvatā dvādasāyatanāni—",
  "pp1.1:4.3_0": "cakkhāyatanaṁ,",
  "pp1.1:4.3_1": "rūpāyatanaṁ,",
  "pp1.1:4.3_2": "sotāyatanaṁ,",
  "pp1.1:4.3_3": "saddāyatanaṁ,",
  "pp1.1:4.3_4": "ghānāyatanaṁ,",
  "pp1.1:4.3_5": "gandhāyatanaṁ,",
  "pp1.1:4.3_6": "jivhāyatanaṁ,",
  "pp1.1:4.3_7": "rasāyatanaṁ,",
  "pp1.1:4.3_8": "kāyāyatanaṁ,",
  "pp1.1:4.3_9": "phoṭṭhabbāyatanaṁ,",
  "pp1.1:4.3_10": "manāyatanaṁ,",
  "pp1.1:4.3_11": "dhammāyatanaṁ;",
  "pp1.1:4.4_0": "ettāvatā āyatanānaṁ āyatanapaññatti.",
  "pp1.1:5.1_0": "Kittāvatā dhātūnaṁ dhātupaññatti?",
  "pp1.1:5.2_0": "Yāvatā aṭṭhārasa dhātuyo—",
  "pp1.1:5.3_0": "cakkhudhātu,",
  "pp1.1:5.3_1": "rūpadhātu,",
  "pp1.1:5.3_2": "cakkhuviññāṇadhātu,",
  "pp1.1:5.3_3": "sotadhātu,",
  "pp1.1:5.3_4": "saddadhātu,",
  "pp1.1:5.3_5": "sotaviññāṇadhātu,",
  "pp1.1:5.3_6": "ghānadhātu,",
  "pp1.1:5.3_7": "gandhadhātu,",
  "pp1.1:5.3_8": "ghānaviññāṇadhātu,",
  "pp1.1:5.3_9": "jivhādhātu,",
  "pp1.1:5.3_10": "rasadhātu,",
  "pp1.1:5.3_11": "jivhāviññāṇadhātu,",
  "pp1.1:5.3_12": "kāyadhātu,",
  "pp1.1:5.3_13": "phoṭṭhabbadhātu,",
  "pp1.1:5.3_14": "kāyaviññāṇadhātu,",
  "pp1.1:5.3_15": "manodhātu,",
  "pp1.1:5.3_16": "dhammadhātu,",
  "pp1.1:5.3_17": "manoviññāṇadhātu;",
  "pp1.1:5.4_0": "ettāvatā dhātūnaṁ dhātupaññatti.",
  "pp1.1:6.1_0": "Kittāvatā saccānaṁ saccapaññatti?",
  "pp1.1:6.2_0": "Yāvatā cattāri saccāni—",
  "pp1.1:6.3_0": "dukkhasaccaṁ, samudayasaccaṁ, nirodhasaccaṁ, maggasaccaṁ;",
  "pp1.1:6.4_0": "ettāvatā saccānaṁ saccapaññatti.",
  "pp1.1:7.1_0": "Kittāvatā indriyānaṁ indriyapaññatti?",
  "pp1.1:7.2_0": "Yāvatā bāvīsatindriyāni—",
  "pp1.1:7.3_0": "cakkhundriyaṁ,",
  "pp1.1:7.3_1": "sotindriyaṁ,",
  "pp1.1:7.3_2": "ghānindriyaṁ,",
  "pp1.1:7.3_3": "jivhindriyaṁ,",
  "pp1.1:7.3_4": "kāyindriyaṁ,",
  "pp1.1:7.3_5": "manindriyaṁ,",
  "pp1.1:7.3_6": "itthindriyaṁ,",
  "pp1.1:7.3_7": "purisindriyaṁ,",
  "pp1.1:7.3_8": "jīvitindriyaṁ,",
  "pp1.1:7.3_9": "sukhindriyaṁ,",
  "pp1.1:7.3_10": "dukkhindriyaṁ,",
  "pp1.1:7.3_11": "somanassindriyaṁ,",
  "pp1.1:7.3_12": "domanassindriyaṁ,",
  "pp1.1:7.3_13": "upekkhindriyaṁ,",
  "pp1.1:7.3_14": "saddhindriyaṁ,",
  "pp1.1:7.3_15": "vīriyindriyaṁ,",
  "pp1.1:7.3_16": "satindriyaṁ,",
  "pp1.1:7.3_17": "samādhindriyaṁ,",
  "pp1.1:7.3_18": "paññindriyaṁ,",
  "pp1.1:7.3_19": "anaññātaññassāmītindriyaṁ,",
  "pp1.1:7.3_20": "aññindriyaṁ,",
  "pp1.1:7.3_21": "aññātāvindriyaṁ;",
  "pp1.1:7.4_0": "ettāvatā indriyānaṁ indriyapaññatti.",
  "pp1.1:8.1_0": "**Kittāvatā puggalānaṁ puggalapaññatti?**",
