{
  "pli-tv-pvr13:0.1_0": "Parivāra",
  "pli-tv-pvr13:0.2_0": "Codanākaṇḍa",
  "pli-tv-pvr13:0.3_0": "1. Anuvijjakaanuyoga",
  "pli-tv-pvr13:1.1_0": "Anuvijjakena codako pucchitabbo—",
  "pli-tv-pvr13:1.2_0": "“yaṁ kho tvaṁ, āvuso, imaṁ bhikkhuṁ codesi, kimhi naṁ codesi, sīlavipattiyā vā codesi, ācāravipattiyā vā codesi, diṭṭhivipattiyā vā codesī”ti?",
  "pli-tv-pvr13:1.3_0": "So ce evaṁ vadeyya—",
  "pli-tv-pvr13:1.4_0": "“sīlavipattiyā vā codemi, ācāravipattiyā vā codemi,",
  "pli-tv-pvr13:1.4_1": "diṭṭhivipattiyā vā codemī”ti, so evamassa vacanīyo—",
  "pli-tv-pvr13:1.5_0": "“jānāsi panāyasmā sīlavipattiṁ, jānāsi ācāravipattiṁ, jānāsi diṭṭhivipattin”ti?",
  "pli-tv-pvr13:1.6_0": "So ce evaṁ vadeyya—",
  "pli-tv-pvr13:1.7_0": "“jānāmi kho ahaṁ, āvuso, sīlavipattiṁ, jānāmi ācāravipattiṁ, jānāmi diṭṭhivipattin”ti, so evamassa vacanīyo—",
  "pli-tv-pvr13:1.8_0": "“katamā panāvuso, sīlavipatti?",
  "pli-tv-pvr13:1.9_0": "Katamā ācāravipatti?",
  "pli-tv-pvr13:1.10_0": "Katamā diṭṭhivipattī”ti?",
  "pli-tv-pvr13:1.11_0": "So ce evaṁ vadeyya—",
  "pli-tv-pvr13:1.12_0": "“cattāri ca pārājikāni, terasa ca saṅghādisesā, ayaṁ sīlavipatti.",
  "pli-tv-pvr13:1.13_0": "Thullaccayaṁ, pācittiyaṁ, pāṭidesanīyaṁ, dukkaṭaṁ, dubbhāsitaṁ, ayaṁ ācāravipatti.",
  "pli-tv-pvr13:1.14_0": "Micchādiṭṭhi, antaggāhikā diṭṭhi, ayaṁ diṭṭhivipattī”ti, so evamassa vacanīyo—",
  "pli-tv-pvr13:1.15_0": "“yaṁ kho tvaṁ, āvuso, imaṁ bhikkhuṁ codesi,",
  "pli-tv-pvr13:1.15_1": "diṭṭhena vā codesi sutena vā codesi parisaṅkāya vā codesī”ti?",
  "pli-tv-pvr13:1.16_0": "So ce evaṁ vadeyya—",
  "pli-tv-pvr13:1.17_0": "“diṭṭhena vā codemi sutena vā codemi parisaṅkāya vā codemī”ti, so evamassa vacanīyo—",
  "pli-tv-pvr13:1.18_0": "“yaṁ kho tvaṁ, āvuso,",
  "pli-tv-pvr13:1.18_1": "imaṁ bhikkhuṁ diṭṭhena codesi,",
  "pli-tv-pvr13:1.18_2": "kiṁ te diṭṭhaṁ kinti te diṭṭhaṁ, kadā te diṭṭhaṁ,",
  "pli-tv-pvr13:1.18_3": "kattha te diṭṭhaṁ pārājikaṁ ajjhāpajjanto diṭṭho,",
  "pli-tv-pvr13:1.18_4": "saṅghādisesaṁ ajjhāpajjanto diṭṭho, thullaccayaṁ …",
  "pli-tv-pvr13:1.19_0": "pācittiyaṁ …",
  "pli-tv-pvr13:1.20_0": "pāṭidesanīyaṁ …",
  "pli-tv-pvr13:1.21_0": "dukkaṭaṁ …",
  "pli-tv-pvr13:1.22_0": "dubbhāsitaṁ ajjhāpajjanto diṭṭho, kattha ca tvaṁ ahosi, kattha cāyaṁ bhikkhu ahosi, kiñca tvaṁ karosi, kiṁ cāyaṁ bhikkhu karotī”ti?",
  "pli-tv-pvr13:1.23_0": "So ce evaṁ vadeyya—",
  "pli-tv-pvr13:1.24_0": "“na kho ahaṁ, āvuso,",
  "pli-tv-pvr13:1.24_1": "imaṁ bhikkhuṁ diṭṭhena codemi, api ca sutena codemī”ti, so evamassa vacanīyo—",
  "pli-tv-pvr13:1.25_0": "“yaṁ kho tvaṁ, āvuso, imaṁ bhikkhuṁ sutena codesi, kiṁ te sutaṁ, kinti te sutaṁ, kadā te sutaṁ, kattha te sutaṁ,",
  "pli-tv-pvr13:1.25_1": "pārājikaṁ ajjhāpannoti sutaṁ, saṅghādisesaṁ …",
  "pli-tv-pvr13:1.26_0": "thullaccayaṁ …",
  "pli-tv-pvr13:1.27_0": "pācittiyaṁ …",
  "pli-tv-pvr13:1.28_0": "pāṭidesanīyaṁ …",
  "pli-tv-pvr13:1.29_0": "dukkaṭaṁ …",
  "pli-tv-pvr13:1.30_0": "dubbhāsitaṁ ajjhāpannoti sutaṁ, bhikkhussa sutaṁ, bhikkhuniyā sutaṁ, sikkhamānāya sutaṁ, sāmaṇerassa sutaṁ, sāmaṇeriyā sutaṁ, upāsakassa sutaṁ, upāsikāya sutaṁ, rājūnaṁ sutaṁ, rājamahāmattānaṁ sutaṁ, titthiyānaṁ sutaṁ, titthiyasāvakānaṁ sutan”ti?",
  "pli-tv-pvr13:1.31_0": "So ce evaṁ vadeyya—",
  "pli-tv-pvr13:1.32_0": "“na kho ahaṁ, āvuso, imaṁ bhikkhuṁ sutena codemi,",
  "pli-tv-pvr13:1.32_1": "api ca parisaṅkāya codemī”ti, so evamassa vacanīyo—",
  "pli-tv-pvr13:1.33_0": "“yaṁ kho tvaṁ, āvuso,",
  "pli-tv-pvr13:1.33_1": "imaṁ bhikkhuṁ parisaṅkāya codesi, kiṁ parisaṅkasi, kinti parisaṅkasi, kadā parisaṅkasi, kattha parisaṅkasi,",
  "pli-tv-pvr13:1.33_2": "pārājikaṁ ajjhāpannoti parisaṅkasi,",
  "pli-tv-pvr13:1.33_3": "saṅghādisesaṁ ajjhāpannoti parisaṅkasi, thullaccayaṁ …",
  "pli-tv-pvr13:1.34_0": "pācittiyaṁ …",
  "pli-tv-pvr13:1.35_0": "pāṭidesanīyaṁ …",
  "pli-tv-pvr13:1.36_0": "dukkaṭaṁ …",
  "pli-tv-pvr13:1.37_0": "dubbhāsitaṁ ajjhāpannoti parisaṅkasi,",
  "pli-tv-pvr13:1.37_1": "bhikkhussa sutvā parisaṅkasi,",
  "pli-tv-pvr13:1.37_2": "bhikkhuniyā sutvā parisaṅkasi,",
  "pli-tv-pvr13:1.37_3": "sikkhamānāya sutvā parisaṅkasi,",
  "pli-tv-pvr13:1.37_4": "sāmaṇerassa sutvā parisaṅkasi,",
  "pli-tv-pvr13:1.37_5": "sāmaṇeriyā sutvā parisaṅkasi,",
  "pli-tv-pvr13:1.37_6": "upāsakassa sutvā parisaṅkasi, upāsikāya sutvā parisaṅkasi, rājūnaṁ sutvā parisaṅkasi,",
  "pli-tv-pvr13:1.37_7": "rājamahāmattānaṁ sutvā parisaṅkasi,",
  "pli-tv-pvr13:1.37_8": "titthiyānaṁ sutvā parisaṅkasi,",
  "pli-tv-pvr13:1.37_9": "titthiyasāvakānaṁ sutvā parisaṅkasī”ti?",
  "pli-tv-pvr13:2.1_0": "Diṭṭhaṁ diṭṭhena sameti,",
  "pli-tv-pvr13:2.2_0": "diṭṭhena saṁsandate diṭṭhaṁ;",
  "pli-tv-pvr13:2.3_0": "Diṭṭhaṁ paṭicca na upeti,",
  "pli-tv-pvr13:2.4_0": "asuddhaparisaṅkito;",
  "pli-tv-pvr13:2.5_0": "So puggalo paṭiññāya,",
  "pli-tv-pvr13:2.6_0": "kātabbo tenuposatho.",
  "pli-tv-pvr13:3.1_0": "Sutaṁ sutena sameti,",
  "pli-tv-pvr13:3.2_0": "sutena saṁsandate sutaṁ;",
  "pli-tv-pvr13:3.3_0": "Sutaṁ paṭicca na upeti,",
  "pli-tv-pvr13:3.4_0": "asuddhaparisaṅkito;",
  "pli-tv-pvr13:3.5_0": "So puggalo paṭiññāya,",
  "pli-tv-pvr13:3.6_0": "kātabbo tenuposatho.",
  "pli-tv-pvr13:4.1_0": "Mutaṁ mutena sameti,",
  "pli-tv-pvr13:4.2_0": "mutena saṁsandate mutaṁ;",
  "pli-tv-pvr13:4.3_0": "Mutaṁ paṭicca na upeti,",
  "pli-tv-pvr13:4.4_0": "asuddhaparisaṅkito;",
  "pli-tv-pvr13:4.5_0": "So puggalo paṭiññāya,",
  "pli-tv-pvr13:4.6_0": "kātabbo tenuposatho.",
  "pli-tv-pvr13:5.1_0": "Codanāya ko ādi, kiṁ majjhe, kiṁ pariyosānaṁ?",
  "pli-tv-pvr13:5.2_0": "Codanāya okāsakammaṁ ādi, kiriyā majjhe, samatho pariyosānaṁ.",
  "pli-tv-pvr13:5.3_0": "Codanāya kati mūlāni, kati vatthūni, kati bhūmiyo, katihākārehi codeti?",
  "pli-tv-pvr13:5.4_0": "Codanāya dve mūlāni, tīṇi vatthūni, pañca bhūmiyo, dvīhākārehi codeti.",
  "pli-tv-pvr13:5.5_0": "Codanāya katamāni dve mūlāni?",
  "pli-tv-pvr13:5.6_0": "Samūlikā vā amūlikā vā—",
  "pli-tv-pvr13:5.7_0": "codanāya imāni dve mūlāni.",
  "pli-tv-pvr13:5.8_0": "Codanāya katamāni tīṇi vatthūni?",
  "pli-tv-pvr13:5.9_0": "Diṭṭhena sutena parisaṅkāya—",
  "pli-tv-pvr13:5.10_0": "codanāya imāni tīṇi vatthūni.",
  "pli-tv-pvr13:5.11_0": "Codanāya katamā pañca bhūmiyo?",
  "pli-tv-pvr13:5.12_0": "Kālena vakkhāmi no akālena,",
  "pli-tv-pvr13:5.12_1": "bhūtena vakkhāmi no abhūtena,",
  "pli-tv-pvr13:5.12_2": "saṇhena vakkhāmi no pharusena,",
  "pli-tv-pvr13:5.12_3": "atthasaṁhitena vakkhāmi no anatthasaṁhitena,",
  "pli-tv-pvr13:5.12_4": "mettācitto vakkhāmi no dosantaroti—",
  "pli-tv-pvr13:5.13_0": "codanāya imā pañca bhūmiyo.",
  "pli-tv-pvr13:6.1_0": "Katamehi dvīhākārehi codeti?",
  "pli-tv-pvr13:6.2_0": "Kāyena vā codeti vācāya vā codeti—",
  "pli-tv-pvr13:6.3_0": "imehi dvīhākārehi codeti.",
  "pli-tv-pvr13:7.0_0": "2. Codakādipaṭipatti",
  "pli-tv-pvr13:7.1_0": "Codakena kathaṁ paṭipajjitabbaṁ?",
  "pli-tv-pvr13:7.2_0": "Cuditakena kathaṁ paṭipajjitabbaṁ?",
  "pli-tv-pvr13:7.3_0": "Saṅghena kathaṁ paṭipajjitabbaṁ?",
  "pli-tv-pvr13:7.4_0": "Anuvijjakena kathaṁ paṭipajjitabbaṁ?",
  "pli-tv-pvr13:7.5_0": "Codakena kathaṁ paṭipajjitabbanti?",
  "pli-tv-pvr13:7.6_0": "Codakena pañcasu dhammesu patiṭṭhāya paro codetabbo.",
  "pli-tv-pvr13:7.7_0": "Kālena vakkhāmi no akālena,",
  "pli-tv-pvr13:7.7_1": "bhūtena vakkhāmi no abhūtena,",
  "pli-tv-pvr13:7.7_2": "saṇhena vakkhāmi no pharusena,",
  "pli-tv-pvr13:7.7_3": "atthasaṁhitena vakkhāmi no anatthasaṁhitena,",
  "pli-tv-pvr13:7.7_4": "mettācitto vakkhāmi no dosantaroti—",
  "pli-tv-pvr13:7.8_0": "codakena evaṁ paṭipajjitabbaṁ.",
  "pli-tv-pvr13:7.9_0": "Cuditakena kathaṁ paṭipajjitabbanti?",
  "pli-tv-pvr13:7.10_0": "Cuditakena dvīsu dhammesu paṭipajjitabbaṁ.",
  "pli-tv-pvr13:7.11_0": "Sacce ca akuppe ca—",
  "pli-tv-pvr13:7.12_0": "cuditakena evaṁ paṭipajjitabbaṁ.",
  "pli-tv-pvr13:7.13_0": "Saṅghena kathaṁ paṭipajjitabbanti?",
  "pli-tv-pvr13:7.14_0": "Saṅghena otiṇṇānotiṇṇaṁ jānitabbaṁ.",
  "pli-tv-pvr13:7.15_0": "Saṅghena evaṁ paṭipajjitabbaṁ.",
  "pli-tv-pvr13:7.16_0": "Anuvijjakena kathaṁ paṭipajjitabbanti?",
  "pli-tv-pvr13:7.17_0": "Anuvijjakena yena dhammena yena vinayena yena satthusāsanena taṁ adhikaraṇaṁ vūpasammati tathā taṁ adhikaraṇaṁ vūpasametabbaṁ.",
  "pli-tv-pvr13:7.18_0": "Anuvijjakena evaṁ paṭipajjitabbaṁ.",
  "pli-tv-pvr13:8.1_0": "Uposatho kimatthāya,",
  "pli-tv-pvr13:8.2_0": "Pavāraṇā kissa kāraṇā;",
  "pli-tv-pvr13:8.3_0": "Parivāso kimatthāya,",
  "pli-tv-pvr13:8.4_0": "Mūlāyapaṭikassanā kissa kāraṇā;",
  "pli-tv-pvr13:8.5_0": "Mānattaṁ kimatthāya,",
  "pli-tv-pvr13:8.6_0": "Abbhānaṁ kissa kāraṇā.",
  "pli-tv-pvr13:9.1_0": "Uposatho sāmaggatthāya,",
  "pli-tv-pvr13:9.2_0": "Visuddhatthāya pavāraṇā;",
  "pli-tv-pvr13:9.3_0": "Parivāso mānattatthāya,",
  "pli-tv-pvr13:9.4_0": "Mūlāyapaṭikassanā niggahatthāya;",
  "pli-tv-pvr13:9.5_0": "Mānattaṁ abbhānatthāya,",
  "pli-tv-pvr13:9.6_0": "Visuddhatthāya abbhānaṁ.",
  "pli-tv-pvr13:10.1_0": "Chandā dosā bhayā mohā,",
  "pli-tv-pvr13:10.2_0": "there ca paribhāsati;",
  "pli-tv-pvr13:10.3_0": "Kāyassa bhedā duppañño,",
  "pli-tv-pvr13:10.4_0": "khato upahatindriyo;",
  "pli-tv-pvr13:10.5_0": "Nirayaṁ gacchati dummedho,",
  "pli-tv-pvr13:10.6_0": "na ca sikkhāya gāravo.",
  "pli-tv-pvr13:11.1_0": "Na ca āmisaṁ nissāya,",
  "pli-tv-pvr13:11.2_0": "na ca nissāya puggalaṁ;",
  "pli-tv-pvr13:11.3_0": "Ubho ete vivajjetvā,",
  "pli-tv-pvr13:11.4_0": "yathādhammo tathā kare.",
  "pli-tv-pvr13:12.0_0": "3. Codakassaattajhāpana",
  "pli-tv-pvr13:12.1_0": "Kodhano upanāhī ca,",
  "pli-tv-pvr13:12.2_0": "Caṇḍo ca paribhāsako;",
  "pli-tv-pvr13:12.3_0": "Anāpattiyā āpattīti ropeti,",
  "pli-tv-pvr13:12.4_0": "Tādiso codako jhāpeti attānaṁ.",
  "pli-tv-pvr13:13.1_0": "Upakaṇṇakaṁ jappati jimhaṁ pekkhati,",
  "pli-tv-pvr13:13.2_0": "Vītiharati kummaggaṁ paṭisevati;",
  "pli-tv-pvr13:13.3_0": "Anāpattiyā āpattīti ropeti,",
  "pli-tv-pvr13:13.4_0": "Tādiso codako jhāpeti attānaṁ.",
  "pli-tv-pvr13:14.1_0": "Akālena codeti abhūtena,",
  "pli-tv-pvr13:14.2_0": "Pharusena anatthasaṁhitena;",
  "pli-tv-pvr13:14.3_0": "Dosantaro codeti no mettācitto,",
  "pli-tv-pvr13:14.4_0": "Anāpattiyā āpattīti ropeti;",
  "pli-tv-pvr13:14.5_0": "Tādiso codako jhāpeti attānaṁ.",
  "pli-tv-pvr13:15.1_0": "Dhammādhammaṁ na jānāti,",
  "pli-tv-pvr13:15.2_0": "Dhammādhammassa akovido;",
  "pli-tv-pvr13:15.3_0": "Anāpattiyā āpattīti ropeti,",
  "pli-tv-pvr13:15.4_0": "Tādiso codako jhāpeti attānaṁ.",
  "pli-tv-pvr13:16.1_0": "Vinayāvinayaṁ na jānāti,",
  "pli-tv-pvr13:16.2_0": "Vinayāvinayassa akovido;",
  "pli-tv-pvr13:16.3_0": "Anāpattiyā āpattīti ropeti,",
  "pli-tv-pvr13:16.4_0": "Tādiso codako jhāpeti attānaṁ.",
  "pli-tv-pvr13:17.1_0": "Bhāsitābhāsitaṁ na jānāti,",
  "pli-tv-pvr13:17.2_0": "Bhāsitābhāsitassa akovido;",
  "pli-tv-pvr13:17.3_0": "Anāpattiyā āpattīti ropeti,",
  "pli-tv-pvr13:17.4_0": "Tādiso codako jhāpeti attānaṁ.",
  "pli-tv-pvr13:18.1_0": "Āciṇṇānāciṇṇaṁ na jānāti,",
  "pli-tv-pvr13:18.2_0": "Āciṇṇānāciṇṇassa akovido;",
  "pli-tv-pvr13:18.3_0": "Anāpattiyā āpattīti ropeti,",
  "pli-tv-pvr13:18.4_0": "Tādiso codako jhāpeti attānaṁ.",
  "pli-tv-pvr13:19.1_0": "Paññattāpaññattaṁ na jānāti,",
  "pli-tv-pvr13:19.2_0": "Paññattāpaññattassa akovido;",
  "pli-tv-pvr13:19.3_0": "Anāpattiyā āpattīti ropeti,",
  "pli-tv-pvr13:19.4_0": "Tādiso codako jhāpeti attānaṁ.",
  "pli-tv-pvr13:20.1_0": "Āpattānāpattiṁ na jānāti,",
  "pli-tv-pvr13:20.2_0": "Āpattānāpattiyā akovido;",
  "pli-tv-pvr13:20.3_0": "Anāpattiyā āpattīti ropeti,",
  "pli-tv-pvr13:20.4_0": "Tādiso codako jhāpeti attānaṁ.",
  "pli-tv-pvr13:21.1_0": "Lahukagarukaṁ na jānāti,",
  "pli-tv-pvr13:21.2_0": "Lahukagarukassa akovido;",
  "pli-tv-pvr13:21.3_0": "Anāpattiyā āpattīti ropeti,",
  "pli-tv-pvr13:21.4_0": "Tādiso codako jhāpeti attānaṁ.",
  "pli-tv-pvr13:22.1_0": "Sāvasesānavasesaṁ na jānāti,",
  "pli-tv-pvr13:22.2_0": "Sāvasesānavasesassa akovido;",
  "pli-tv-pvr13:22.3_0": "Anāpattiyā āpattīti ropeti,",
  "pli-tv-pvr13:22.4_0": "Tādiso codako jhāpeti attānaṁ.",
  "pli-tv-pvr13:23.1_0": "Duṭṭhullāduṭṭhullaṁ na jānāti,",
  "pli-tv-pvr13:23.2_0": "Duṭṭhullāduṭṭhullassa akovido;",
  "pli-tv-pvr13:23.3_0": "Anāpattiyā āpattīti ropeti,",
  "pli-tv-pvr13:23.4_0": "Tādiso codako jhāpeti attānaṁ.",
  "pli-tv-pvr13:24.1_0": "Pubbāparaṁ na jānāti,",
  "pli-tv-pvr13:24.2_0": "Pubbāparassa akovido;",
  "pli-tv-pvr13:24.3_0": "Anāpattiyā āpattīti ropeti,",
  "pli-tv-pvr13:24.4_0": "Tādiso codako jhāpeti attānaṁ.",
  "pli-tv-pvr13:25.1_0": "Anusandhivacanapathaṁ na jānāti,",
  "pli-tv-pvr13:25.2_0": "Anusandhivacanapathassa akovido;",
  "pli-tv-pvr13:25.3_0": "Anāpattiyā āpattīti ropeti,",
  "pli-tv-pvr13:25.4_0": "Tādiso codako jhāpeti attānanti.",
  "pli-tv-pvr13:26.1_0": "Codanākaṇḍaṁ niṭṭhitaṁ.",
  "pli-tv-pvr13:27.1_0": "Tassuddānaṁ",
  "pli-tv-pvr13:28.1_0": "Codanā anuvijjā ca,",
  "pli-tv-pvr13:28.2_0": "ādi mūlenuposatho;",
  "pli-tv-pvr13:28.3_0": "Gati codanakaṇḍamhi,",
  "pli-tv-pvr13:28.4_0": "sāsanaṁ patiṭṭhāpayanti."
}