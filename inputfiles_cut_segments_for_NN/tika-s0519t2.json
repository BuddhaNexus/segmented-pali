{
  "tika-s0519t2:0_0": "2. Uddesavāravaṇṇanā",
  "tika-s0519t2:1_0": "1.Vibhāgenāti sarūpavibhāgena.",
  "tika-s0519t2:1_1": "Adiṭṭhaṁ jotīyati etāyāti adiṭṭhajotanā.",
  "tika-s0519t2:1_2": "Diṭṭhaṁ saṁsandīyati etāyāti diṭṭhasaṁsandanā,",
  "tika-s0519t2:1_3": "saṁsandanaṁ cettha sākacchāvasena vinicchayakaraṇaṁ.",
  "tika-s0519t2:1_4": "Vimati chijjati etāyāti vimaticchedanā.",
  "tika-s0519t2:1_5": "Anumatiyā pucchā anumatipucchā.",
  "tika-s0519t2:1_6": "‘‘Taṁ kiṁ maññathā’’ti hi kā tumhākaṁ anumatīti anumati pucchitā.",
  "tika-s0519t2:1_7": "Kathetukamyatāti kathetukamyatāya.",
  "tika-s0519t2:2_0": "‘‘Harīyanti etehī’’tiādinā karaṇādhikaraṇakattubhāvakammasādhanānaṁ vasena hāra-saddassa atthaṁ vatvā",
  "tika-s0519t2:2_1": "sadisakappanāvasena dassetuṁ ‘‘hārā viyā’’tiādi vuttaṁ.",
  "tika-s0519t2:2_2": "Puna ganthakaraṇādiatthena ganthādisaddānaṁ viya hārakaraṇādiatthena hārasaddasiddhiṁ dassetuṁ ‘‘hārayantī’’tiādimāha.",
  "tika-s0519t2:2_3": "‘‘Haraṇato,",
  "tika-s0519t2:2_4": "ramaṇato cā’’ti iminā manoharā manoramā cete saṁvaṇṇanāvisesāti dasseti.",
  "tika-s0519t2:3_0": "Upapattisādhanayuttīti lakkhaṇahetu.",
  "tika-s0519t2:3_1": "Vuttanayenāti ‘‘nanu ca aññepi hārā yuttisahitā evā’’tiādinā desanāhāre vuttanayānusārena.",
  "tika-s0519t2:4_0": "Catunnaṁ byūho etthāti bhinnādhikaraṇānampi padānaṁ aññapadatthasamāso labbhati ‘‘urasilomo’’tiādīnaṁ (dī. ni. ṭī. 3.54,",
  "tika-s0519t2:4_1": "303) viyāti vuttaṁ.",
  "tika-s0519t2:5_0": "Sesanti ‘‘vivacanameva vevacana’’nti evamādi.",
  "tika-s0519t2:6_0": "Anuppavesīyantīti avagāhīyanti.",
  "tika-s0519t2:6_1": "Samādhīyantīti pariharīyanti.",
  "tika-s0519t2:6_2": "Vinā vikappenāti jāti sāmaññaṁ,",
  "tika-s0519t2:6_3": "bhedo sāmaññaṁ,",
  "tika-s0519t2:6_4": "sambandho sāmaññantiādinā padatthantarabhāvavikappanamantarena.",
  "tika-s0519t2:7_0": "Padaṭṭhānādimukhenāti padaṭṭhānavevacanabhāvanāpahānamukhena.",
  "tika-s0519t2:7_1": "Kecīti padaṭṭhānaparikkhāraāvaṭṭaparivattanapaññattiotaraṇe sandhāya vadati.",
  "tika-s0519t2:8_0": "2.Sambandhoti hetuphalabhāvayogo.",
  "tika-s0519t2:8_1": "Tathābhūtānañhi dhammānaṁ ekasantānasiddhatā ekattanayo.",
  "tika-s0519t2:8_2": "Vibhāgo satipi nesaṁ hetuphalabhāve vibhattasabhāvatā.",
  "tika-s0519t2:8_3": "Añño eva hi hetu,",
  "tika-s0519t2:8_4": "aññaṁ phalanti.",
  "tika-s0519t2:8_5": "Byāpāraviraho nirīhatā.",
  "tika-s0519t2:8_6": "Na hi hetuphalānaṁ evaṁ hoti ‘‘ahaṁ imaṁ nibbattemi,",
  "tika-s0519t2:8_7": "imināhaṁ nibbatto’’ti.",
  "tika-s0519t2:8_8": "Anurūpaphalatā paccayuppannānaṁ paccayānukūlatā.",
  "tika-s0519t2:8_9": "Samūhādiṁ upādāya lokasaṅketasiddhā vohāramattatā sammutisabhāvo.",
  "tika-s0519t2:8_10": "Pathavīphassādīnaṁ kakkhaḷaphusanādilakkhaṇaṁ paramatthasabhāvo.",
  "tika-s0519t2:8_11": "Ayañhettha saṅkhepo –",
  "tika-s0519t2:8_12": "yasmiṁ bhinne,",
  "tika-s0519t2:8_13": "itarāpohe vā cittena katena tathā buddhi,",
  "tika-s0519t2:8_14": "idaṁ sammutisaccaṁ yathā ghaṭe,",
  "tika-s0519t2:8_15": "sasambhārajale ca,",
  "tika-s0519t2:8_16": "tabbipariyāyena paramatthasaccanti.",
  "tika-s0519t2:8_17": "Paramatthasaccappaṭivedhāyāti nibbānādhigamāya.",
  "tika-s0519t2:9_0": "Antoti abbhantaro.",
  "tika-s0519t2:9_1": "Padhānāvayavenāti mūlabhāvena.",
  "tika-s0519t2:9_2": "‘‘Nandī dukkhassa mūla’’ntiādīsu (ma. ni.",
  "tika-s0519t2:9_3": "1.13) taṇhā ‘‘nandī’’ti vuttā.",
  "tika-s0519t2:9_4": "‘‘Saṅgāme ca nandiṁ caratī’’tiādīsu pamodoti āha ‘‘taṇhāya,",
  "tika-s0519t2:9_5": "pamodassa vā’’ti.",
  "tika-s0519t2:10_0": "3.Jātibhedatoti kusalā,",
  "tika-s0519t2:10_1": "akusalāti imasmā visesā.",
  "tika-s0519t2:10_2": "Yujjantīti ettha hetuattho antonīto veditabboti āha ‘‘yojīyantī’’ti.",
  "tika-s0519t2:10_3": "Kehi yojīyanti?",
  "tika-s0519t2:10_4": "Saṁvaṇṇanakehīti adhippāyo.",
  "tika-s0519t2:10_5": "Yujjantīti vā yuttā honti,",
  "tika-s0519t2:10_6": "tehi samānayogakkhamā taggahaṇeneva gahitā hontīti attho tadekaṭṭhabhāvato.",
  "tika-s0519t2:10_7": "Imasmiṁ atthe ‘‘navahi padehī’’ti sahayoge karaṇavacanaṁ,",
  "tika-s0519t2:10_8": "purimasmiṁ karaṇe.",
  "tika-s0519t2:10_9": "‘‘Ete kho’’ti ca pāṭho.",
  "tika-s0519t2:10_10": "Tattha kho-saddassa padapūraṇatā,",
  "tika-s0519t2:10_11": "avadhāraṇatthatā vā veditabbā.",
  "tika-s0519t2:10_12": "Ete evāti ete taṇhādayo eva,",
  "tika-s0519t2:10_13": "na ito aññeti attho.",
  "tika-s0519t2:10_14": "Aṭṭhāraseva na tato uddhaṁ,",
  "tika-s0519t2:10_15": "adho vāti.",
  "tika-s0519t2:10_16": "Purimasmiṁ pakkhe mūlapadantarābhāvo,",
  "tika-s0519t2:10_17": "dutiyasmiṁ tesaṁ anūnādhikatā dīpitā hoti.",
  "tika-s0519t2:11_0": "Uddesavāravaṇṇanā niṭṭhitā."
}