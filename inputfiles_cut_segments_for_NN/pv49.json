{
  "pv49:0.1_0": "Petavatthu",
  "pv49:0.2_0": "Mahāvagga",
  "pv49:0.3_0": "14. Bhogasaṁharaṇapetavatthu",
  "pv49:1.1_0": "“Mayaṁ bhoge saṁharimhā,",
  "pv49:1.2_0": "samena visamena ca;",
  "pv49:1.3_0": "Te aññe paribhuñjanti,",
  "pv49:1.4_0": "mayaṁ dukkhassa bhāginī”ti.",
  "pv49:2.1_0": "Bhogasaṁharaṇapetavatthu cuddasamaṁ."
}