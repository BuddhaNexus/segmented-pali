{
  "atk-s0305a7:0_0": "8. Anuruddhasaṁyuttaṁ",
  "atk-s0305a7:1_0": "1. Rahogatavaggo",
  "atk-s0305a7:2_0": "1-2. Paṭhamarahogatasuttādivaṇṇanā",
  "atk-s0305a7:3_0": "899-900.",
  "atk-s0305a7:3_1": "Anuruddhasaṁyuttassa paṭhame āraddhāti paripuṇṇā.",
  "atk-s0305a7:3_2": "Saṅkhepato panettha chattiṁsāya ṭhānesu arahattaṁ pāpetvā vipassanā kathitā,",
  "atk-s0305a7:3_3": "dutiye dvādasasu ṭhānesu arahattaṁ pāpetvā vipassanā kathitā.",
  "atk-s0305a7:4_0": "3. Sutanusuttavaṇṇanā",
  "atk-s0305a7:5_0": "901. Tatiye mahābhiññatanti chaabhiññābhāvaṁ. Hīnaṁ dhammantiādīsu imāya pāḷiyā attho veditabbo –",
  "atk-s0305a7:6_0": "‘‘Katame dhammā hīnā?",
  "atk-s0305a7:6_1": "Dvādasa akusalacittuppādā,",
  "atk-s0305a7:6_2": "ime dhammā hīnā.",
  "atk-s0305a7:6_3": "Katame dhammā majjhimā?",
  "atk-s0305a7:6_4": "Tīsu bhūmīsu kusalaṁ,",
  "atk-s0305a7:6_5": "tīsu bhūmīsu vipāko,",
  "atk-s0305a7:6_6": "tīsu bhūmīsu kiriyābyākataṁ sabbañca rūpaṁ,",
  "atk-s0305a7:6_7": "ime dhammā majjhimā.",
  "atk-s0305a7:6_8": "Katame dhammā paṇītā?",
  "atk-s0305a7:6_9": "Cattāro maggā apariyāpannā,",
  "atk-s0305a7:6_10": "cattāri ca sāmaññaphalāni nibbānañca,",
  "atk-s0305a7:6_11": "ime dhammā paṇītā’’ti (dha. sa.",
  "atk-s0305a7:6_12": "1423-1425).",
  "atk-s0305a7:7_0": "4-7. Paṭhamakaṇḍakīsuttādivaṇṇanā",
  "atk-s0305a7:8_0": "902-905.",
  "atk-s0305a7:8_1": "Catutthe kaṇḍakīvaneti mahākaramandavane.",
  "atk-s0305a7:8_2": "Chaṭṭhe sahassaṁ lokanti iminā therassa satata vihāro dassito.",
  "atk-s0305a7:8_3": "Thero hi pātova mukhaṁ dhovitvā atītānāgate kappasahassaṁ anussarati.",
  "atk-s0305a7:8_4": "Paccuppanne pana dasacakkavāḷasahassaṁ ekāvajjanassa āpāthamāgacchati.",
  "atk-s0305a7:8_5": "Sattamaṁ uttānameva.",
  "atk-s0305a7:9_0": "8. Salaḷāgārasuttavaṇṇanā",
  "atk-s0305a7:10_0": "906.",
  "atk-s0305a7:10_1": "Aṭṭhame salaḷāgāreti salaḷarukkhamayāya paṇṇasālāya,",
  "atk-s0305a7:10_2": "salaḷarukkhassa vā dvāre ṭhitattā evaṁnāmake agāre.",
  "atk-s0305a7:10_3": "Imasmiṁ sutte vipassanāya saddhiṁ vipassakapuggalo kathito.",
  "atk-s0305a7:11_0": "9. Ambapālivanasuttavaṇṇanā",
  "atk-s0305a7:12_0": "907.",
  "atk-s0305a7:12_1": "Navame āsabhiṁ vācanti attano arahattabhāvadīpakaṁ uttamavācaṁ.",
  "atk-s0305a7:12_2": "Sesaṁ sabbattha uttānatthamevāti.",
  "atk-s0305a7:13_0": "Rahogatavaggo paṭhamo.",
  "atk-s0305a7:14_0": "2. Dutiyavaggavaṇṇanā",
  "atk-s0305a7:15_0": "909-922.",
  "atk-s0305a7:15_1": "Dutiyavagge ṭhānañca ṭhānatotiādīhi thero dasabalañāṇaṁ paṭijānāti.",
  "atk-s0305a7:15_2": "Kimpanetaṁ sāvakānaṁ hotīti?",
  "atk-s0305a7:15_3": "Ekadesena hoti,",
  "atk-s0305a7:15_4": "sabbaññubuddhānaṁ panetaṁ nippadesaṁ sabbākāraparipūranti.",
  "atk-s0305a7:16_0": "Anuruddhasaṁyuttavaṇṇanā niṭṭhitā."
}