{
  "tika-s0303t3:0_0": "4. Okkantasaṁyuttaṁ",
  "tika-s0303t3:1_0": "1-10. Cakkhusuttādivaṇṇanā",
  "tika-s0303t3:2_0": "302-311.Saddhādhimokkhanti saddahanavasena pavattaṁ adhimokkhaṁ,",
  "tika-s0303t3:2_1": "na sanniṭṭhānamattavasena pavattaṁ adhimokkhaṁ.",
  "tika-s0303t3:2_2": "Dassanampi sammattaṁ,",
  "tika-s0303t3:2_3": "taṁsijjhānavasena pavattaniyāmo sammattaniyāmo,",
  "tika-s0303t3:2_4": "ariyamaggo.",
  "tika-s0303t3:2_5": "Anantarāyataṁ dīpeti kappavināsapaṭibhāgena pavattattā.",
  "tika-s0303t3:2_6": "Tathā cāha “tenevāhā”tiādi.",
  "tika-s0303t3:2_7": "Kappasīsena bhājanalokaṁ vadati.",
  "tika-s0303t3:2_8": "So hi uḍḍayhati, na kappo,",
  "tika-s0303t3:2_9": "uḍḍayhanavelāti jhāyanavelā.",
  "tika-s0303t3:2_10": "Ṭhito kappo ṭhitakappo,",
  "tika-s0303t3:2_11": "so assa atthīti ṭhitakappī,",
  "tika-s0303t3:2_12": "kappaṁ ṭhapetuṁ samatthoti attho.",
  "tika-s0303t3:2_13": "Olokananti saccābhisamayasaṅkhātaṁ dassanaṁ.",
  "tika-s0303t3:2_14": "Khamanti sahanti,",
  "tika-s0303t3:2_15": "ñāyantīti attho.",
  "tika-s0303t3:3_0": "Cakkhusuttādivaṇṇanā niṭṭhitā.",
  "tika-s0303t3:4_0": "Okkantasaṁyuttavaṇṇanā niṭṭhitā."
}