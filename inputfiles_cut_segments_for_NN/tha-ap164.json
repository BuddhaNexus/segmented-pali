{
  "tha-ap164:0.1_0": "Therāpadāna",
  "tha-ap164:0.2_0": "Supāricariyavagga",
  "tha-ap164:0.3_0": "2. Kaṇaverapupphiyattheraapadāna",
  "tha-ap164:1.1_0": "“Siddhattho nāma bhagavā,",
  "tha-ap164:1.2_0": "lokajeṭṭho narāsabho;",
  "tha-ap164:1.3_0": "Purakkhato sāvakehi,",
  "tha-ap164:1.4_0": "nagaraṁ paṭipajjatha.",
  "tha-ap164:2.1_0": "Rañño antepure āsiṁ,",
  "tha-ap164:2.2_0": "gopako abhisammato;",
  "tha-ap164:2.3_0": "Pāsāde upaviṭṭhohaṁ,",
  "tha-ap164:2.4_0": "addasaṁ lokanāyakaṁ.",
  "tha-ap164:3.1_0": "Kaṇaveraṁ gahetvāna,",
  "tha-ap164:3.2_0": "bhikkhusaṅghe samokiriṁ;",
  "tha-ap164:3.3_0": "Buddhassa visuṁ katvāna,",
  "tha-ap164:3.4_0": "tato bhiyyo samokiriṁ.",
  "tha-ap164:4.1_0": "Catunnavutito kappe,",
  "tha-ap164:4.2_0": "Yaṁ pupphamabhipūjayiṁ;",
  "tha-ap164:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap164:4.4_0": "Buddhapūjāyidaṁ phalaṁ.",
  "tha-ap164:5.1_0": "Sattāsītimhito kappe,",
  "tha-ap164:5.2_0": "caturāsuṁ mahiddhikā;",
  "tha-ap164:5.3_0": "Sattaratanasampannā,",
  "tha-ap164:5.4_0": "cakkavattī mahabbalā.",
  "tha-ap164:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap164:6.2_0": "…pe…",
  "tha-ap164:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap164:7.1_0": "Itthaṁ sudaṁ āyasmā kaṇaverapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap164:8.1_0": "Kaṇaverapupphiyattherassāpadānaṁ dutiyaṁ."
}