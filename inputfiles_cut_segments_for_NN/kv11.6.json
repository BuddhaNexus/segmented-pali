{
  "kv11.6:0.1_0": "Kathāvatthu",
  "kv11.6:0.2_0": "Mahāpaṇṇāsaka",
  "kv11.6:0.3_0": "Ekādasamavagga",
  "kv11.6:0.4_0": "Samādhikathā",
  "kv11.6:1.1_0": "Cittasantati samādhīti?",
  "kv11.6:1.2_0": "Āmantā.",
  "kv11.6:1.3_0": "Atītā cittasantati samādhīti?",
  "kv11.6:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv11.6:1.5_0": "cittasantati samādhīti?",
  "kv11.6:1.6_0": "Āmantā.",
  "kv11.6:1.7_0": "Anāgatā cittasantati samādhīti?",
  "kv11.6:1.8_0": "Na hevaṁ vattabbe …pe…",
  "kv11.6:1.9_0": "cittasantati samādhīti?",
  "kv11.6:1.10_0": "Āmantā.",
  "kv11.6:1.11_0": "Nanu atītaṁ niruddhaṁ anāgataṁ ajātanti?",
  "kv11.6:1.12_0": "Āmantā.",
  "kv11.6:1.13_0": "Hañci atītaṁ niruddhaṁ anāgataṁ ajātaṁ, no ca vata re vattabbe—",
  "kv11.6:1.14_0": "“cittasantati samādhī”ti.",
  "kv11.6:2.1_0": "Ekacittakkhaṇiko samādhīti?",
  "kv11.6:2.2_0": "Āmantā.",
  "kv11.6:2.3_0": "Cakkhuviññāṇasamaṅgī samāpannoti?",
  "kv11.6:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv11.6:2.5_0": "sotaviññāṇasamaṅgī …pe…",
  "kv11.6:2.6_0": "ghānaviññāṇasamaṅgī …",
  "kv11.6:2.7_0": "jivhāviññāṇasamaṅgī …",
  "kv11.6:2.8_0": "kāyaviññāṇasamaṅgī …pe…",
  "kv11.6:2.9_0": "akusalacittasamaṅgī …pe…",
  "kv11.6:2.10_0": "rāgasahagatacittasamaṅgī …pe…",
  "kv11.6:2.11_0": "dosasahagatacittasamaṅgī …pe…",
  "kv11.6:2.12_0": "mohasahagatacittasamaṅgī …pe…",
  "kv11.6:2.13_0": "anottappasahagatacittasamaṅgī samāpannoti?",
  "kv11.6:2.14_0": "Na hevaṁ vattabbe …pe….",
  "kv11.6:3.1_0": "Cittasantati samādhīti?",
  "kv11.6:3.2_0": "Āmantā.",
  "kv11.6:3.3_0": "Akusalacittasantati samādhīti?",
  "kv11.6:3.4_0": "Na hevaṁ vattabbe …pe…",
  "kv11.6:3.5_0": "rāgasahagatā …pe…",
  "kv11.6:3.6_0": "dosasahagatā …pe…",
  "kv11.6:3.7_0": "mohasahagatā …pe…",
  "kv11.6:3.8_0": "anottappasahagatā cittasantati samādhīti?",
  "kv11.6:3.9_0": "Na hevaṁ vattabbe …pe….",
  "kv11.6:4.1_0": "Na vattabbaṁ—",
  "kv11.6:4.2_0": "“cittasantati samādhī”ti?",
  "kv11.6:4.3_0": "Āmantā.",
  "kv11.6:4.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv11.6:4.5_0": "“ahaṁ kho,",
  "kv11.6:4.5_1": "āvuso nigaṇṭhā,",
  "kv11.6:4.5_2": "pahomi aniñjamāno kāyena,",
  "kv11.6:4.5_3": "abhāsamāno vācaṁ,",
  "kv11.6:4.5_4": "satta rattindivāni ekantasukhaṁ paṭisaṁvedī viharitun”ti.",
  "kv11.6:4.6_0": "Attheva suttantoti?",
  "kv11.6:4.7_0": "Āmantā.",
  "kv11.6:4.8_0": "Tena hi cittasantati samādhīti.",
  "kv11.6:4.9_0": "Samādhikathā niṭṭhitā."
}