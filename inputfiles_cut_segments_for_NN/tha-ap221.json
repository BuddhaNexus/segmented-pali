{
  "tha-ap221:0.1_0": "Therāpadāna",
  "tha-ap221:0.2_0": "Hatthivagga",
  "tha-ap221:0.3_0": "9. Sappidāyakattheraapadāna",
  "tha-ap221:1.1_0": "“Nisinno pāsādavare,",
  "tha-ap221:1.2_0": "nārīgaṇapurakkhato;",
  "tha-ap221:1.3_0": "Byādhitaṁ samaṇaṁ disvā,",
  "tha-ap221:1.4_0": "abhināmesahaṁ gharaṁ.",
  "tha-ap221:2.1_0": "Upaviṭṭhaṁ mahāvīraṁ,",
  "tha-ap221:2.2_0": "devadevaṁ narāsabhaṁ;",
  "tha-ap221:2.3_0": "Sappitelaṁ mayā dinnaṁ,",
  "tha-ap221:2.4_0": "siddhatthassa mahesino.",
  "tha-ap221:3.1_0": "Passaddhadarathaṁ disvā,",
  "tha-ap221:3.2_0": "vippasannamukhindriyaṁ;",
  "tha-ap221:3.3_0": "Vanditvā satthuno pāde,",
  "tha-ap221:3.4_0": "anusaṁsāvayiṁ pure.",
  "tha-ap221:4.1_0": "Disvā maṁ suppasannattaṁ,",
  "tha-ap221:4.2_0": "iddhiyā pāramiṅgato;",
  "tha-ap221:4.3_0": "Nabhaṁ abbhuggamī dhīro,",
  "tha-ap221:4.4_0": "haṁsarājāva ambare.",
  "tha-ap221:5.1_0": "Catunnavutito kappe,",
  "tha-ap221:5.2_0": "yaṁ dānamadadiṁ tadā;",
  "tha-ap221:5.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap221:5.4_0": "sappitelassidaṁ phalaṁ.",
  "tha-ap221:6.1_0": "Ito sattarase kappe,",
  "tha-ap221:6.2_0": "jutideva sanāmako;",
  "tha-ap221:6.3_0": "Sattaratanasampanno,",
  "tha-ap221:6.4_0": "cakkavattī mahabbalo.",
  "tha-ap221:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap221:7.2_0": "…pe…",
  "tha-ap221:7.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap221:8.1_0": "Itthaṁ sudaṁ āyasmā sappidāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap221:9.1_0": "Sappidāyakattherassāpadānaṁ navamaṁ."
}