{
  "kv2.11:0.1_0": "Kathāvatthu",
  "kv2.11:0.2_0": "Mahāpaṇṇāsaka",
  "kv2.11:0.3_0": "Dutiyavagga",
  "kv2.11:0.4_0": "Nirodhakathā",
  "kv2.11:1.1_0": "Dve nirodhāti?",
  "kv2.11:1.2_0": "Āmantā.",
  "kv2.11:1.3_0": "Dve dukkhanirodhāti?",
  "kv2.11:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv2.11:1.5_0": "dve dukkhanirodhāti?",
  "kv2.11:1.6_0": "Āmantā.",
  "kv2.11:1.7_0": "Dve nirodhasaccānīti?",
  "kv2.11:1.8_0": "Na hevaṁ vattabbe …pe…",
  "kv2.11:1.9_0": "dve nirodhasaccānīti?",
  "kv2.11:1.10_0": "Āmantā.",
  "kv2.11:1.11_0": "Dve dukkhasaccānīti?",
  "kv2.11:1.12_0": "Na hevaṁ vattabbe …pe…",
  "kv2.11:1.13_0": "dve nirodhasaccānīti?",
  "kv2.11:1.14_0": "Āmantā.",
  "kv2.11:1.15_0": "Dve samudayasaccānīti?",
  "kv2.11:1.16_0": "Na hevaṁ vattabbe …pe…",
  "kv2.11:1.17_0": "dve nirodhasaccānīti?",
  "kv2.11:1.18_0": "Āmantā.",
  "kv2.11:1.19_0": "Dve maggasaccānīti?",
  "kv2.11:1.20_0": "Na hevaṁ vattabbe …pe….",
  "kv2.11:2.1_0": "Dve nirodhasaccānīti?",
  "kv2.11:2.2_0": "Āmantā.",
  "kv2.11:2.3_0": "Dve tāṇāni …pe…",
  "kv2.11:2.4_0": "dve leṇāni …",
  "kv2.11:2.5_0": "dve saraṇāni …",
  "kv2.11:2.6_0": "dve parāyaṇāni …",
  "kv2.11:2.7_0": "dve accutāni …",
  "kv2.11:2.8_0": "dve amatāni …",
  "kv2.11:2.9_0": "dve nibbānānīti?",
  "kv2.11:2.10_0": "Na hevaṁ vattabbe …pe….",
  "kv2.11:3.1_0": "Dve nibbānānīti?",
  "kv2.11:3.2_0": "Āmantā.",
  "kv2.11:3.3_0": "Atthi dvinnaṁ nibbānānaṁ uccanīcatā hīnapaṇītatā ukkaṁsāvakaṁso sīmā vā bhedo vā rāji vā antarikā vāti?",
  "kv2.11:3.4_0": "Na hevaṁ vattabbe …pe….",
  "kv2.11:4.1_0": "Dve nirodhāti?",
  "kv2.11:4.2_0": "Āmantā.",
  "kv2.11:4.3_0": "Nanu appaṭisaṅkhāniruddhe saṅkhāre paṭisaṅkhā nirodhentīti?",
  "kv2.11:4.4_0": "Āmantā.",
  "kv2.11:4.5_0": "Hañci appaṭisaṅkhāniruddhe saṅkhāre paṭisaṅkhā nirodhenti, no ca vata re vattabbe—",
  "kv2.11:4.6_0": "“dve nirodhā”ti.",
  "kv2.11:5.1_0": "Na vattabbaṁ—",
  "kv2.11:5.2_0": "“dve nirodhā”ti?",
  "kv2.11:5.3_0": "Āmantā.",
  "kv2.11:5.4_0": "Nanu appaṭisaṅkhāniruddhāpi saṅkhārā accantabhaggā,",
  "kv2.11:5.4_1": "paṭisaṅkhāniruddhāpi saṅkhārā accantabhaggāti?",
  "kv2.11:5.5_0": "Āmantā.",
  "kv2.11:5.6_0": "Hañci appaṭisaṅkhāniruddhāpi saṅkhārā accantabhaggā,",
  "kv2.11:5.6_1": "paṭisaṅkhāniruddhāpi saṅkhārā accantabhaggā, tena vata re vattabbe—",
  "kv2.11:5.7_0": "“dve nirodhā”ti.",
  "kv2.11:6.1_0": "Dve nirodhāti?",
  "kv2.11:6.2_0": "Āmantā.",
  "kv2.11:6.3_0": "Paṭisaṅkhāniruddhāpi saṅkhārā ariyamaggaṁ āgamma niruddhāti?",
  "kv2.11:6.4_0": "Āmantā.",
  "kv2.11:6.5_0": "Appaṭisaṅkhāniruddhā saṅkhārā ariyamaggaṁ āgamma niruddhāti?",
  "kv2.11:6.6_0": "Na hevaṁ vattabbe …pe…",
  "kv2.11:7.1_0": "Dve nirodhāti?",
  "kv2.11:7.2_0": "Āmantā.",
  "kv2.11:7.3_0": "Paṭisaṅkhāniruddhā saṅkhārā na puna uppajjantīti?",
  "kv2.11:7.4_0": "Āmantā.",
  "kv2.11:7.5_0": "Appaṭisaṅkhāniruddhā saṅkhārā na puna uppajjantīti?",
  "kv2.11:7.6_0": "Na hevaṁ vattabbe …pe…",
  "kv2.11:7.7_0": "tena hi na vattabbaṁ—",
  "kv2.11:7.8_0": "“dve nirodhā”ti.",
  "kv2.11:7.9_0": "Nirodhakathā niṭṭhitā.",
  "kv2.11:7.10_0": "Dutiyo vaggo.",
  "kv2.11:7.11_0": "Tassuddānaṁ",
  "kv2.11:8.1_0": "Parūpahāro aññāṇaṁ,",
  "kv2.11:8.2_0": "kaṅkhā paravitāraṇā;",
  "kv2.11:8.3_0": "Vacībhedo dukkhāhāro,",
  "kv2.11:8.4_0": "cittaṭṭhiti ca kukkuḷā;",
  "kv2.11:8.5_0": "Anupubbābhisamayo,",
  "kv2.11:8.6_0": "vohāro ca nirodhakoti."
}