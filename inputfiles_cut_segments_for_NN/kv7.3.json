{
  "kv7.3:0.1_0": "Kathāvatthu",
  "kv7.3:0.2_0": "Mahāpaṇṇāsaka",
  "kv7.3:0.3_0": "Sattamavagga",
  "kv7.3:0.4_0": "Cetasikakathā",
  "kv7.3:1.1_0": "Natthi cetasiko dhammoti?",
  "kv7.3:1.2_0": "Āmantā.",
  "kv7.3:1.3_0": "Nanu atthi keci dhammā cittena sahagatā sahajātā saṁsaṭṭhā sampayuttā ekuppādā ekanirodhā ekavatthukā ekārammaṇāti?",
  "kv7.3:1.4_0": "Āmantā.",
  "kv7.3:1.5_0": "Hañci atthi keci dhammā cittena sahagatā sahajātā saṁsaṭṭhā sampayuttā ekuppādā ekanirodhā ekavatthukā ekārammaṇā, no ca vata re vattabbe—",
  "kv7.3:1.6_0": "“natthi cetasiko dhammo”ti.",
  "kv7.3:2.1_0": "Phasso cittena sahajātoti?",
  "kv7.3:2.2_0": "Āmantā.",
  "kv7.3:2.3_0": "Hañci phasso cittena sahajāto, tena vata re vattabbe—",
  "kv7.3:2.4_0": "“phasso cetasiko”ti.",
  "kv7.3:2.5_0": "Vedanā …pe…",
  "kv7.3:2.6_0": "saññā …",
  "kv7.3:2.7_0": "cetanā …",
  "kv7.3:2.8_0": "saddhā …",
  "kv7.3:2.9_0": "vīriyaṁ …",
  "kv7.3:2.10_0": "sati …",
  "kv7.3:2.11_0": "samādhi …",
  "kv7.3:2.12_0": "paññā …",
  "kv7.3:2.13_0": "rāgo …",
  "kv7.3:2.14_0": "doso …",
  "kv7.3:2.15_0": "moho …pe…",
  "kv7.3:2.16_0": "anottappaṁ cittena sahajātanti?",
  "kv7.3:2.17_0": "Āmantā.",
  "kv7.3:2.18_0": "Hañci anottappaṁ cittena sahajātaṁ, tena vata re vattabbe—",
  "kv7.3:2.19_0": "“anottappaṁ cetasikan”ti.",
  "kv7.3:3.1_0": "Cittena sahajātāti katvā cetasikāti?",
  "kv7.3:3.2_0": "Āmantā.",
  "kv7.3:3.3_0": "Phassena sahajātāti katvā phassasikāti?",
  "kv7.3:3.4_0": "Āmantā.",
  "kv7.3:3.5_0": "Cittena sahajātāti katvā cetasikāti?",
  "kv7.3:3.6_0": "Āmantā.",
  "kv7.3:3.7_0": "Vedanāya …",
  "kv7.3:3.8_0": "saññāya …",
  "kv7.3:3.9_0": "cetanāya …",
  "kv7.3:3.10_0": "saddhāya …",
  "kv7.3:3.11_0": "vīriyena …",
  "kv7.3:3.12_0": "satiyā …",
  "kv7.3:3.13_0": "samādhinā …",
  "kv7.3:3.14_0": "paññāya …",
  "kv7.3:3.15_0": "rāgena …",
  "kv7.3:3.16_0": "dosena …",
  "kv7.3:3.17_0": "mohena …pe…",
  "kv7.3:3.18_0": "anottappena sahajātāti katvā anottappāsikāti?",
  "kv7.3:3.19_0": "Āmantā.",
  "kv7.3:4.1_0": "Natthi cetasiko dhammoti?",
  "kv7.3:4.2_0": "Āmantā.",
  "kv7.3:4.3_0": "Nanu vuttaṁ bhagavatā—",
  "kv7.3:5.1_0": "“Cittañhidaṁ cetasikā ca dhammā,",
  "kv7.3:5.2_0": "Anattato saṁviditassa honti;",
  "kv7.3:5.3_0": "Hīnappaṇītaṁ tadubhaye viditvā,",
  "kv7.3:5.4_0": "Sammaddaso vedi palokadhamman”ti.",
  "kv7.3:6.1_0": "Attheva suttantoti?",
  "kv7.3:6.2_0": "Āmantā.",
  "kv7.3:6.3_0": "Tena hi atthi cetasiko dhammoti.",
  "kv7.3:7.1_0": "Natthi cetasiko dhammoti?",
  "kv7.3:7.2_0": "Āmantā.",
  "kv7.3:7.3_0": "Nanu vuttaṁ bhagavatā—",
  "kv7.3:7.4_0": "“idha, kevaṭṭa,",
  "kv7.3:7.4_1": "bhikkhu parasattānaṁ parapuggalānaṁ cittampi ādisati cetasikampi ādisati vitakkitampi ādisati vicāritampi ādisati—",
  "kv7.3:7.5_0": "‘evampi te mano, itthampi te mano, itipi te cittan’”ti.",
  "kv7.3:7.6_0": "Attheva suttantoti?",
  "kv7.3:7.7_0": "Āmantā.",
  "kv7.3:7.8_0": "Tena hi atthi cetasiko dhammoti.",
  "kv7.3:7.9_0": "Cetasikakathā niṭṭhitā."
}