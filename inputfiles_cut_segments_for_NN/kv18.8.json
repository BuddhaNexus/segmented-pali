{
  "kv18.8:0.1_0": "Kathāvatthu",
  "kv18.8:0.2_0": "Mahāpaṇṇāsaka",
  "kv18.8:0.3_0": "Aṭṭhārasamavagga",
  "kv18.8:0.4_0": "Saddaṁsuṇātītikathā",
  "kv18.8:1.1_0": "Samāpanno saddaṁ suṇātīti?",
  "kv18.8:1.2_0": "Āmantā.",
  "kv18.8:1.3_0": "Samāpanno cakkhunā rūpaṁ passati …pe…",
  "kv18.8:1.4_0": "sotena …pe…",
  "kv18.8:1.5_0": "ghānena …pe…",
  "kv18.8:1.6_0": "jivhāya …pe…",
  "kv18.8:1.7_0": "kāyena phoṭṭhabbaṁ phusatīti?",
  "kv18.8:1.8_0": "Na hevaṁ vattabbe …pe….",
  "kv18.8:2.1_0": "Samāpanno saddaṁ suṇātīti?",
  "kv18.8:2.2_0": "Āmantā.",
  "kv18.8:2.3_0": "Sotaviññāṇasamaṅgī samāpannoti?",
  "kv18.8:2.4_0": "Na hevaṁ vattabbe.",
  "kv18.8:2.5_0": "Nanu samādhi manoviññāṇasamaṅgissāti?",
  "kv18.8:2.6_0": "Āmantā.",
  "kv18.8:2.7_0": "Hañci samādhi manoviññāṇasamaṅgissa, no ca vata re vattabbe—",
  "kv18.8:2.8_0": "“samāpanno saddaṁ suṇātī”ti.",
  "kv18.8:3.1_0": "Samādhi manoviññāṇasamaṅgissa, sotaviññāṇasamaṅgī saddaṁ suṇātīti?",
  "kv18.8:3.2_0": "Āmantā.",
  "kv18.8:3.3_0": "Hañci samādhi manoviññāṇasamaṅgissa, sotaviññāṇasamaṅgī saddaṁ suṇāti, no ca vata re vattabbe—",
  "kv18.8:3.4_0": "“samāpanno saddaṁ suṇātī”ti.",
  "kv18.8:3.5_0": "Samādhi manoviññāṇasamaṅgissa, sotaviññāṇasamaṅgī saddaṁ suṇātīti?",
  "kv18.8:3.6_0": "Āmantā.",
  "kv18.8:3.7_0": "Dvinnaṁ phassānaṁ …pe…",
  "kv18.8:3.8_0": "dvinnaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv18.8:3.9_0": "Na hevaṁ vattabbe …pe….",
  "kv18.8:4.1_0": "Na vattabbaṁ—",
  "kv18.8:4.2_0": "“samāpanno saddaṁ suṇātī”ti?",
  "kv18.8:4.3_0": "Āmantā.",
  "kv18.8:4.4_0": "Nanu paṭhamassa jhānassa saddo kaṇṭako vutto bhagavatāti?",
  "kv18.8:4.5_0": "Āmantā.",
  "kv18.8:4.6_0": "Hañci paṭhamassa jhānassa saddo kaṇṭako vutto bhagavatā, tena vata re vattabbe—",
  "kv18.8:4.7_0": "“samāpanno saddaṁ suṇātī”ti.",
  "kv18.8:5.1_0": "Paṭhamassa jhānassa saddo kaṇṭako vutto bhagavatāti, samāpanno saddaṁ suṇātīti?",
  "kv18.8:5.2_0": "Āmantā.",
  "kv18.8:5.3_0": "Dutiyassa jhānassa vitakko vicāro kaṇṭako vutto bhagavatā, atthi tassa vitakkavicārāti?",
  "kv18.8:5.4_0": "Na hevaṁ vattabbe …pe….",
  "kv18.8:6.1_0": "Paṭhamassa jhānassa saddo kaṇṭako vutto bhagavatāti, samāpanno saddaṁ suṇātīti?",
  "kv18.8:6.2_0": "Āmantā.",
  "kv18.8:6.3_0": "Tatiyassa jhānassa pīti kaṇṭako …pe…",
  "kv18.8:6.4_0": "catutthassa jhānassa assāsapassāso kaṇṭako …",
  "kv18.8:6.4_1": "ākāsānañcāyatanaṁ samāpannassa rūpasaññā kaṇṭako …",
  "kv18.8:6.4_2": "viññāṇañcāyatanaṁ samāpannassa ākāsānañcāyatanasaññā kaṇṭako …",
  "kv18.8:6.4_3": "ākiñcaññāyatanaṁ samāpannassa viññāṇañcāyatanasaññā kaṇṭako …",
  "kv18.8:6.4_4": "nevasaññānāsaññāyatanaṁ samāpannassa ākiñcaññāyatanasaññā kaṇṭako …",
  "kv18.8:6.4_5": "saññāvedayitanirodhaṁ samāpannassa saññā ca vedanā ca kaṇṭako vutto bhagavatā,",
  "kv18.8:6.4_6": "atthi tassa saññā ca vedanā cāti?",
  "kv18.8:6.5_0": "Na hevaṁ vattabbe …pe….",
  "kv18.8:6.6_0": "Saddaṁ suṇātītikathā niṭṭhitā."
}