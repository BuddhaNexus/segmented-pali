{
  "kv9.8:0.1_0": "Kathāvatthu",
  "kv9.8:0.2_0": "Mahāpaṇṇāsaka",
  "kv9.8:0.3_0": "Navamavagga",
  "kv9.8:0.4_0": "Vitakkavipphārasaddakathā",
  "kv9.8:1.1_0": "Sabbaso vitakkayato vicārayato vitakkavipphāro saddoti?",
  "kv9.8:1.2_0": "Āmantā.",
  "kv9.8:1.3_0": "Sabbaso phusayato phassavipphāro saddo,",
  "kv9.8:1.3_1": "sabbaso vedayato vedanāvipphāro saddo,",
  "kv9.8:1.3_2": "sabbaso sañjānato saññāvipphāro saddo,",
  "kv9.8:1.3_3": "sabbaso cetayato cetanāvipphāro saddo,",
  "kv9.8:1.3_4": "sabbaso cintayato cittavipphāro saddo,",
  "kv9.8:1.3_5": "sabbaso sarato sativipphāro saddo,",
  "kv9.8:1.3_6": "sabbaso pajānato paññāvipphāro saddoti?",
  "kv9.8:1.4_0": "Na hevaṁ vattabbe …pe….",
  "kv9.8:2.1_0": "Sabbaso vitakkayato vicārayato vitakkavipphāro saddoti?",
  "kv9.8:2.2_0": "Āmantā.",
  "kv9.8:2.3_0": "Vitakkavipphāro saddo sotaviññeyyo sotasmiṁ paṭihaññati sotassa āpāthaṁ āgacchatīti?",
  "kv9.8:2.4_0": "Na hevaṁ vattabbe …pe….",
  "kv9.8:2.5_0": "Nanu vitakkavipphāro saddo na sotaviññeyyo na sotasmiṁ paṭihaññati na sotassa āpāthaṁ āgacchatīti?",
  "kv9.8:2.6_0": "Āmantā.",
  "kv9.8:2.7_0": "Hañci vitakkavipphāro saddo na sotaviññeyyo na sotasmiṁ paṭihaññati na sotassa āpāthaṁ āgacchati, no ca vata re vattabbe—",
  "kv9.8:2.8_0": "“sabbaso vitakkayato vicārayato vitakkavipphāro saddo”ti.",
  "kv9.8:2.9_0": "Vitakkavipphārasaddakathā niṭṭhitā."
}