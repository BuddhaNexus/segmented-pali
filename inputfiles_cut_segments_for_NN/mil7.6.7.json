{
  "mil7.6.7:0.1_0": "Milindapañha",
  "mil7.6.7:0.2_0": "Opammakathāpañha",
  "mil7.6.7:0.3_0": "Sīhavagga",
  "mil7.6.7:0.4_0": "7. Vagguliṅgapañha",
  "mil7.6.7:1.1_0": "“Bhante nāgasena,",
  "mil7.6.7:1.1_1": "‘vaggulissa dve aṅgāni gahetabbānī’ti yaṁ vadesi,",
  "mil7.6.7:1.1_2": "katamāni tāni dve aṅgāni gahetabbānī”ti?",
  "mil7.6.7:1.2_0": "“Yathā, mahārāja,",
  "mil7.6.7:1.2_1": "vagguli gehaṁ pavisitvā vicaritvā nikkhamati, na tattha palibuddhati;",
  "mil7.6.7:1.3_0": "evameva kho, mahārāja,",
  "mil7.6.7:1.3_1": "yoginā yogāvacarena gāmaṁ piṇḍāya pavisitvā sapadānaṁ vicaritvā paṭiladdhalābhena khippameva nikkhamitabbaṁ,",
  "mil7.6.7:1.3_2": "na tattha palibuddhena bhavitabbaṁ.",
  "mil7.6.7:1.4_0": "Idaṁ, mahārāja,",
  "mil7.6.7:1.4_1": "vaggulissa paṭhamaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.6.7:2.1_0": "Puna caparaṁ, mahārāja,",
  "mil7.6.7:2.1_1": "vagguli paragehe vasamāno na tesaṁ parihāniṁ karoti;",
  "mil7.6.7:2.2_0": "evameva kho, mahārāja,",
  "mil7.6.7:2.2_1": "yoginā yogāvacarena kulāni upasaṅkamitvā atiyācanāya vā viññattibahulatāya vā kāyadosabahulatāya vā atibhāṇitāya vā samānasukhadukkhatāya vā na tesaṁ koci vippaṭisāro karaṇīyo,",
  "mil7.6.7:2.2_2": "napi tesaṁ mūlakammaṁ parihāpetabbaṁ,",
  "mil7.6.7:2.2_3": "sabbathā vaḍḍhiyeva icchitabbā.",
  "mil7.6.7:2.3_0": "Idaṁ, mahārāja,",
  "mil7.6.7:2.3_1": "vaggulissa dutiyaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.6.7:2.4_0": "Bhāsitampetaṁ, mahārāja,",
  "mil7.6.7:2.4_1": "bhagavatā devātidevena dīghanikāyavare lakkhaṇasuttante—",
  "mil7.6.7:3.1_0": "‘Suddhāya sīlena sutena buddhiyā,",
  "mil7.6.7:3.2_0": "Cāgena dhammena bahūhi sādhuhi;",
  "mil7.6.7:3.3_0": "Dhanena dhaññena ca khettavatthunā,",
  "mil7.6.7:3.4_0": "Puttehi dārehi catuppadehi ca.",
  "mil7.6.7:4.1_0": "Ñātīhi mittehi ca bandhavehi,",
  "mil7.6.7:4.2_0": "Balena vaṇṇena sukhena cūbhayaṁ;",
  "mil7.6.7:4.3_0": "Kathaṁ na hāyeyyuṁ pareti icchati,",
  "mil7.6.7:4.4_0": "Atthasamiddhiñca panābhikaṅkhatī’”ti.",
  "mil7.6.7:5.1_0": "Vagguliṅgapañho sattamo."
}