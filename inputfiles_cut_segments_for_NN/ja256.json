{
  "ja256:0.1_0": "Jātaka",
  "ja256:0.2_0": "Tikanipāta",
  "ja256:0.3_0": "Saṅkappavagga",
  "ja256:0.4_0": "6. Jarūdapānajātaka",
  "ja256:1.1_0": "“Jarūdapānaṁ khaṇamānā,",
  "ja256:1.2_0": "vāṇijā udakatthikā;",
  "ja256:1.3_0": "Ajjhagamuṁ ayasaṁ lohaṁ,",
  "ja256:1.4_0": "tipusīsañca vāṇijā;",
  "ja256:1.5_0": "Rajataṁ jātarūpañca,",
  "ja256:1.6_0": "muttā veḷuriyā bahū.",
  "ja256:2.1_0": "Te ca tena asantuṭṭhā,",
  "ja256:2.2_0": "bhiyyo bhiyyo akhāṇisuṁ;",
  "ja256:2.3_0": "Te tatthāsīviso ghoro,",
  "ja256:2.4_0": "tejassī tejasā hani.",
  "ja256:3.1_0": "Tasmā khaṇe nātikhaṇe,",
  "ja256:3.2_0": "Atikhātaṁ hi pāpakaṁ;",
  "ja256:3.3_0": "Khātena ca dhanaṁ laddhaṁ,",
  "ja256:3.4_0": "Atikhātena nāsitan”ti.",
  "ja256:4.1_0": "Jarūdapānajātakaṁ chaṭṭhaṁ."
}