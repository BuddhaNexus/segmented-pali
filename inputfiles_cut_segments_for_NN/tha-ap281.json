{
  "tha-ap281:0.1_0": "Therāpadāna",
  "tha-ap281:0.2_0": "Suvaṇṇabibbohanavagga",
  "tha-ap281:0.3_0": "9 Ekapuṇḍarīkattheraapadāna",
  "tha-ap281:1.1_0": "“Romaso nāma nāmena,",
  "tha-ap281:1.2_0": "sayambhū subbato tadā;",
  "tha-ap281:1.3_0": "Puṇḍarīkaṁ mayā dinnaṁ,",
  "tha-ap281:1.4_0": "vippasannena cetasā.",
  "tha-ap281:2.1_0": "Catunnavutito kappe,",
  "tha-ap281:2.2_0": "puṇḍarīkamadaṁ tadā;",
  "tha-ap281:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap281:2.4_0": "puṇḍarīkassidaṁ phalaṁ.",
  "tha-ap281:3.1_0": "Paṭisambhidā catasso,",
  "tha-ap281:3.2_0": "…pe…",
  "tha-ap281:3.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap281:4.1_0": "Itthaṁ sudaṁ āyasmā ekapuṇḍarīko thero imā gāthāyo abhāsitthāti.",
  "tha-ap281:5.1_0": "Ekapuṇḍarīkattherassāpadānaṁ navamaṁ."
}