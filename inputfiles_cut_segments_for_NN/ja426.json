{
  "ja426:0.1_0": "Jātaka",
  "ja426:0.2_0": "Aṭṭhakanipāta",
  "ja426:0.3_0": "Kaccānivagga",
  "ja426:0.4_0": "10. Dīpijātaka",
  "ja426:1.1_0": "“Khamanīyaṁ yāpanīyaṁ,",
  "ja426:1.2_0": "kacci mātula te sukhaṁ;",
  "ja426:1.3_0": "Sukhaṁ te ammā avaca,",
  "ja426:1.4_0": "sukhakāmāva te mayaṁ”.",
  "ja426:2.1_0": "“Naṅguṭṭhaṁ me avakkamma,",
  "ja426:2.2_0": "Heṭhayitvāna eḷike;",
  "ja426:2.3_0": "Sājja mātulavādena,",
  "ja426:2.4_0": "Muñcitabbā nu maññasi”.",
  "ja426:3.1_0": "“Puratthāmukho nisinnosi,",
  "ja426:3.2_0": "Ahaṁ te mukhamāgatā;",
  "ja426:3.3_0": "Pacchato tuyhaṁ naṅguṭṭhaṁ,",
  "ja426:3.4_0": "Kathaṁ khvāhaṁ avakkamiṁ”.",
  "ja426:4.1_0": "“Yāvatā caturo dīpā,",
  "ja426:4.2_0": "sasamuddā sapabbatā;",
  "ja426:4.3_0": "Tāvatā mayhaṁ naṅguṭṭhaṁ,",
  "ja426:4.4_0": "kathaṁ kho tvaṁ vivajjayi”.",
  "ja426:5.1_0": "“Pubbeva metamakkhiṁsu,",
  "ja426:5.2_0": "mātā pitā ca bhātaro;",
  "ja426:5.3_0": "Dīghaṁ duṭṭhassa naṅguṭṭhaṁ,",
  "ja426:5.4_0": "sāmhi vehāyasāgatā”.",
  "ja426:6.1_0": "“Tañca disvāna āyantiṁ,",
  "ja426:6.2_0": "antalikkhasmi eḷike;",
  "ja426:6.3_0": "Migasaṅgho palāyittha,",
  "ja426:6.4_0": "bhakkho me nāsito tayā”.",
  "ja426:7.1_0": "“Iccevaṁ vilapantiyā,",
  "ja426:7.2_0": "eḷakiyā ruhagghaso;",
  "ja426:7.3_0": "Galakaṁ anvāvamaddi,",
  "ja426:7.4_0": "natthi duṭṭhe subhāsitaṁ.",
  "ja426:8.1_0": "Neva duṭṭhe nayo atthi,",
  "ja426:8.2_0": "Na dhammo na subhāsitaṁ;",
  "ja426:8.3_0": "Nikkamaṁ duṭṭhe yuñjetha,",
  "ja426:8.4_0": "So ca sabbhiṁ na rañjatī”ti. ",
  "ja426:9.1_0": "Dīpijātakaṁ dasamaṁ.",
  "ja426:10.1_0": "Aṭṭhakanipātaṁ niṭṭhitaṁ.",
  "ja426:11.1_0": "Tassuddānaṁ",
  "ja426:12.1_0": "Parisuddhā manāvilā vatthadharā,",
  "ja426:12.2_0": "Bakarājassa kāyuraṁ daṇḍavaro;",
  "ja426:12.3_0": "Atha aṅgāra cetiya devilinā,",
  "ja426:12.4_0": "Atha āditta gaṅgā daseḷakināti."
}