{
  "iti45:0.1_0": "Itivuttaka 45",
  "iti45:0.2_0": "Dukanipāta",
  "iti45:0.3_0": "Dutiyavagga",
  "iti45:0.4_0": "Paṭisallānasutta",
  "iti45:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti45:2.1_0": "“Paṭisallānārāmā,",
  "iti45:2.1_1": "bhikkhave,",
  "iti45:2.1_2": "viharatha paṭisallānaratā,",
  "iti45:2.1_3": "ajjhattaṁ cetosamathamanuyuttā,",
  "iti45:2.1_4": "anirākatajjhānā,",
  "iti45:2.1_5": "vipassanāya samannāgatā,",
  "iti45:2.1_6": "brūhetāro suññāgārānaṁ.",
  "iti45:2.2_0": "Paṭisallānārāmānaṁ,",
  "iti45:2.2_1": "bhikkhave,",
  "iti45:2.2_2": "viharataṁ paṭisallānaratānaṁ ajjhattaṁ cetosamathamanuyuttānaṁ anirākatajjhānānaṁ vipassanāya samannāgatānaṁ",
  "iti45:2.2_3": "brūhetānaṁ suññāgārānaṁ dvinnaṁ phalānaṁ aññataraṁ phalaṁ pāṭikaṅkhaṁ—",
  "iti45:2.3_0": "diṭṭheva dhamme aññā, sati vā upādisese anāgāmitā”ti.",
  "iti45:2.4_0": "Etamatthaṁ bhagavā avoca.",
  "iti45:2.5_0": "Tatthetaṁ iti vuccati:",
  "iti45:3.1_0": "“Ye santacittā nipakā,",
  "iti45:3.2_0": "satimanto ca jhāyino;",
  "iti45:3.3_0": "Sammā dhammaṁ vipassanti,",
  "iti45:3.4_0": "kāmesu anapekkhino.",
  "iti45:4.1_0": "Appamādaratā santā,",
  "iti45:4.2_0": "pamāde bhayadassino;",
  "iti45:4.3_0": "Abhabbā parihānāya,",
  "iti45:4.4_0": "nibbānasseva santike”ti.",
  "iti45:5.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti45:5.2_0": "Aṭṭhamaṁ."
}