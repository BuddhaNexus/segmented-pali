{
  "pli-tv-bi-vb-pc61:0.1_0": "Theravāda Vinaya",
  "pli-tv-bi-vb-pc61:0.2_0": "Bhikkhunivibhaṅga",
  "pli-tv-bi-vb-pc61:0.3_0": "Pācittiyakaṇḍa",
  "pli-tv-bi-vb-pc61:0.4_0": "Gabbhinivagga",
  "pli-tv-bi-vb-pc61:0.5_0": "61. Gabbhinisikkhāpada",
  "pli-tv-bi-vb-pc61:1.1_0": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bi-vb-pc61:1.2_0": "Tena kho pana samayena bhikkhuniyo gabbhiniṁ vuṭṭhāpenti.",
  "pli-tv-bi-vb-pc61:1.3_0": "Sā piṇḍāya carati.",
  "pli-tv-bi-vb-pc61:1.4_0": "Manussā evamāhaṁsu—",
  "pli-tv-bi-vb-pc61:1.5_0": "“dethāyyāya bhikkhaṁ, garubhārā ayyā”ti.",
  "pli-tv-bi-vb-pc61:1.6_0": "Manussā ujjhāyanti khiyyanti vipācenti—",
  "pli-tv-bi-vb-pc61:1.7_0": "“kathañhi nāma bhikkhuniyo gabbhiniṁ vuṭṭhāpessantī”ti.",
  "pli-tv-bi-vb-pc61:1.8_0": "Assosuṁ kho bhikkhuniyo tesaṁ manussānaṁ ujjhāyantānaṁ khiyyantānaṁ vipācentānaṁ.",
  "pli-tv-bi-vb-pc61:1.9_0": "Yā tā bhikkhuniyo appicchā …pe… tā ujjhāyanti khiyyanti vipācenti—",
  "pli-tv-bi-vb-pc61:1.10_0": "“kathañhi nāma bhikkhuniyo gabbhiniṁ vuṭṭhāpessantī”ti …pe…",
  "pli-tv-bi-vb-pc61:1.11_0": "“saccaṁ kira, bhikkhave,",
  "pli-tv-bi-vb-pc61:1.11_1": "bhikkhuniyo gabbhiniṁ vuṭṭhāpentī”ti?",
  "pli-tv-bi-vb-pc61:1.12_0": "“Saccaṁ, bhagavā”ti.",
  "pli-tv-bi-vb-pc61:1.13_0": "Vigarahi buddho bhagavā …pe…",
  "pli-tv-bi-vb-pc61:1.14_0": "kathañhi nāma, bhikkhave,",
  "pli-tv-bi-vb-pc61:1.14_1": "bhikkhuniyo gabbhiniṁ vuṭṭhāpessanti.",
  "pli-tv-bi-vb-pc61:1.15_0": "Netaṁ, bhikkhave, appasannānaṁ vā pasādāya …pe…",
  "pli-tv-bi-vb-pc61:1.16_0": "evañca pana, bhikkhave,",
  "pli-tv-bi-vb-pc61:1.16_1": "bhikkhuniyo imaṁ sikkhāpadaṁ uddisantu—",
  "pli-tv-bi-vb-pc61:1.17.1_0": "“Yā pana bhikkhunī gabbhiniṁ vuṭṭhāpeyya, pācittiyan”ti.",
  "pli-tv-bi-vb-pc61:2.1.1_0": "Yā panāti",
  "pli-tv-bi-vb-pc61:2.1.2_0": "yā yādisā …pe…",
  "pli-tv-bi-vb-pc61:2.1.3_0": "bhikkhunīti",
  "pli-tv-bi-vb-pc61:2.1.4_0": "…pe… ayaṁ imasmiṁ atthe adhippetā bhikkhunīti.",
  "pli-tv-bi-vb-pc61:2.1.5_0": "Gabbhinī nāma",
  "pli-tv-bi-vb-pc61:2.1.6_0": "āpannasattā vuccati.",
  "pli-tv-bi-vb-pc61:2.1.7_0": "Vuṭṭhāpeyyāti",
  "pli-tv-bi-vb-pc61:2.1.8_0": "upasampādeyya.",
  "pli-tv-bi-vb-pc61:2.1.9_0": "“Vuṭṭhāpessāmī”ti gaṇaṁ vā ācariniṁ vā pattaṁ vā cīvaraṁ vā pariyesati, sīmaṁ vā sammannati, āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc61:2.1.10_0": "Ñattiyā dukkaṭaṁ.",
  "pli-tv-bi-vb-pc61:2.1.11_0": "Dvīhi kammavācāhi dukkaṭā.",
  "pli-tv-bi-vb-pc61:2.1.12_0": "Kammavācāpariyosāne upajjhāyāya āpatti pācittiyassa.",
  "pli-tv-bi-vb-pc61:2.1.13_0": "Gaṇassa ca ācariniyā ca āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc61:2.2.1_0": "Gabbhiniyā gabbhinisaññā vuṭṭhāpeti, āpatti pācittiyassa.",
  "pli-tv-bi-vb-pc61:2.2.2_0": "Gabbhiniyā vematikā vuṭṭhāpeti, āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc61:2.2.3_0": "Gabbhiniyā agabbhinisaññā vuṭṭhāpeti, anāpatti.",
  "pli-tv-bi-vb-pc61:2.2.4_0": "Agabbhiniyā gabbhinisaññā, āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc61:2.2.5_0": "Agabbhiniyā vematikā, āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc61:2.2.6_0": "Agabbhiniyā agabbhinisaññā, anāpatti.",
  "pli-tv-bi-vb-pc61:2.3.1_0": "Anāpatti—",
  "pli-tv-bi-vb-pc61:2.3.2_0": "gabbhiniṁ agabbhinisaññā vuṭṭhāpeti,",
  "pli-tv-bi-vb-pc61:2.3.3_0": "agabbhiniṁ agabbhinisaññā vuṭṭhāpeti,",
  "pli-tv-bi-vb-pc61:2.3.4_0": "ummattikāya,",
  "pli-tv-bi-vb-pc61:2.3.5_0": "ādikammikāyāti.",
  "pli-tv-bi-vb-pc61:2.3.6_0": "Paṭhamasikkhāpadaṁ niṭṭhitaṁ."
}