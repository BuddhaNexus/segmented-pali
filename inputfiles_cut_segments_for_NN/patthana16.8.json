{
  "patthana16.8:0.1_0": "Paṭṭhānapakaraṇa",
  "patthana16.8:0.2_0": "Dhammānulomapaccanīya (3), Tikadukapaṭṭhānapāḷi (4)",
  "patthana16.8:0.3_0": "16.8. Kusalattika, Sanidassanaduka",
  "patthana16.8:1.1_0": "Abyākato sanidassano dhammo naabyākatassa nasanidassanassa dhammassa ārammaṇapaccayena paccayo.",
  "patthana16.8:1.1_1": "Abyākato sanidassano dhammo nakusalassa nasanidassanassa …pe….",
  "patthana16.8:1.1_2": "(Cha pañhā kātabbā.) Ārammaṇe cha,",
  "patthana16.8:1.1_3": "adhipatiyā tīṇi,",
  "patthana16.8:1.1_4": "upanissaye purejāte cha,",
  "patthana16.8:1.1_5": "atthiyā avigate cha.",
  "patthana16.8:2.1_0": "Kusalaṁ anidassanaṁ dhammaṁ paṭicca nakusalo naanidassano dhammo uppajjati hetupaccayā.",
  "patthana16.8:2.1_1": "Akusalena tīṇiyeva.",
  "patthana16.8:2.1_2": "Abyākatena tīṇiyeva.",
  "patthana16.8:2.1_3": "Hetuyā nava …pe…",
  "patthana16.8:2.1_4": "vipāke tīṇi …pe…",
  "patthana16.8:2.1_5": "avigate nava."
}