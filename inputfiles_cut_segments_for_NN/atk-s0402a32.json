{
  "atk-s0402a32:0_0": "(14) 4. Yodhājīvavaggo",
  "atk-s0402a32:1_0": "1. Yodhājīvasuttavaṇṇanā",
  "atk-s0402a32:2_0": "134.",
  "atk-s0402a32:2_1": "Catutthassa paṭhame yuddhaṁ upajīvatīti yodhājīvo.",
  "atk-s0402a32:2_2": "Rājārahoti rañño anucchaviko.",
  "atk-s0402a32:2_3": "Rājabhoggoti rañño upabhogaparibhogo.",
  "atk-s0402a32:2_4": "Aṅganteva saṅkhyaṁ gacchatīti hattho viya pādo viya ca avassaṁ icchitabbattā aṅganti saṅkhyaṁ gacchati.",
  "atk-s0402a32:2_5": "Dūre pātī hotīti udake usabhamattaṁ ,",
  "atk-s0402a32:2_6": "thale aṭṭhusabhamattaṁ,",
  "atk-s0402a32:2_7": "tato vā uttarinti dūre kaṇḍaṁ pāteti.",
  "atk-s0402a32:2_8": "Duṭṭhagāmaṇiabhayassa hi yodhājīvo navausabhamattaṁ kaṇḍaṁ pātesi,",
  "atk-s0402a32:2_9": "pacchimabhave bodhisatto yojanappamāṇaṁ.",
  "atk-s0402a32:2_10": "Akkhaṇavedhīti avirādhitavedhī,",
  "atk-s0402a32:2_11": "akkhaṇaṁ vā vijju vijjantarikāya vijjhituṁ samatthoti attho.",
  "atk-s0402a32:2_12": "Mahato kāyassa padāletāti ekatobaddhaṁ phalakasatampi mahiṁsacammasatampi aṅguṭṭhapamāṇabahalaṁ lohapaṭṭampi",
  "atk-s0402a32:2_13": "caturaṅgulabahalaṁ asanapadarampi vidatthibahalaṁ udumbarapadarampi dīghantena vālikasakaṭampi vinivijjhituṁ",
  "atk-s0402a32:2_14": "samatthoti attho.",
  "atk-s0402a32:2_15": "Yaṁkiñci rūpantiādi visuddhimagge vitthāritameva.",
  "atk-s0402a32:2_16": "Netaṁ mamātiādi taṇhāmānadiṭṭhipaṭikkhepavasena vuttaṁ.",
  "atk-s0402a32:2_17": "Sammappaññāya passatīti sammā hetunā kāraṇena sahavipassanāya maggapaññāya passati.",
  "atk-s0402a32:2_18": "Padāletīti arahattamaggena padāleti.",
  "atk-s0402a32:3_0": "2. Parisāsuttavaṇṇanā",
  "atk-s0402a32:4_0": "135.",
  "atk-s0402a32:4_1": "Dutiye ukkācitavinītāti appaṭipucchitvā vinītā dubbinītaparisā.",
  "atk-s0402a32:4_2": "Paṭipucchāvinītāti pucchitvā vinītā suvinītaparisā.",
  "atk-s0402a32:4_3": "Yāvatāvinītāti pamāṇavasena vinītā,",
  "atk-s0402a32:4_4": "pamāṇaṁ ñatvā vinītaparisāti attho.",
  "atk-s0402a32:4_5": "‘‘Yāvatajjhā’’ti pāḷiyā pana yāva ajjhāsayāti attho,",
  "atk-s0402a32:4_6": "ajjhāsayaṁ ñatvā vinītaparisāti vuttaṁ hoti.",
  "atk-s0402a32:4_7": "Tatiyaṁ uttānameva.",
  "atk-s0402a32:5_0": "4. Uppādāsuttavaṇṇanā",
  "atk-s0402a32:6_0": "137.",
  "atk-s0402a32:6_1": "Catutthe dhammaṭṭhitatāti sabhāvaṭṭhitatā.",
  "atk-s0402a32:6_2": "Dhammaniyāmatāti sabhāvaniyāmatā.",
  "atk-s0402a32:6_3": "Sabbe saṅkhārāti catubhūmakasaṅkhārā.",
  "atk-s0402a32:6_4": "Aniccāti hutvā abhāvaṭṭhena aniccā.",
  "atk-s0402a32:6_5": "Dukkhāti sampaṭipīḷanaṭṭhena dukkhā.",
  "atk-s0402a32:6_6": "Anattāti avasavattanaṭṭhena anattā.",
  "atk-s0402a32:6_7": "Iti imasmiṁ sutte tīṇi lakkhaṇāni missakāni kathitāni.",
  "atk-s0402a32:7_0": "5. Kesakambalasuttavaṇṇanā",
  "atk-s0402a32:8_0": "138.",
  "atk-s0402a32:8_1": "Pañcame tantāvutānaṁ vatthānanti paccatte sāmivacanaṁ,",
  "atk-s0402a32:8_2": "tantehi vāyitavatthānīti attho.",
  "atk-s0402a32:8_3": "Kesakambaloti manussakesehi vāyitakambalo .",
  "atk-s0402a32:8_4": "Puthusamaṇabrāhmaṇavādānanti idampi paccatte sāmivacanaṁ.",
  "atk-s0402a32:8_5": "Paṭikiṭṭhoti pacchimako lāmako.",
  "atk-s0402a32:8_6": "Moghapurisoti tucchapuriso.",
  "atk-s0402a32:8_7": "Paṭibāhatīti paṭisedheti.",
  "atk-s0402a32:8_8": "Khippaṁ uḍḍeyyāti kuminaṁ oḍḍeyya.",
  "atk-s0402a32:8_9": "Chaṭṭhasattamāni uttānatthāneva.",
  "atk-s0402a32:9_0": "8. Assakhaḷuṅkasuttavaṇṇanā",
  "atk-s0402a32:10_0": "141.",
  "atk-s0402a32:10_1": "Aṭṭhame assakhaḷuṅkoti assapoto.",
  "atk-s0402a32:10_2": "Idamassa javasmiṁ vadāmīti ayamassa ñāṇajavoti vadāmi.",
  "atk-s0402a32:10_3": "Idamassa vaṇṇasmiṁ vadāmīti ayamassa guṇavaṇṇoti vadāmi.",
  "atk-s0402a32:10_4": "Idamassa ārohapariṇāhasminti ayamassa uccabhāvo parimaṇḍalabhāvoti vadāmīti.",
  "atk-s0402a32:11_0": "9. Assaparassasuttavaṇṇanā",
  "atk-s0402a32:12_0": "142.",
  "atk-s0402a32:12_1": "Navame assaparasseti assesu parasse.",
  "atk-s0402a32:12_2": "Purisaparasseti purisesu parasse,",
  "atk-s0402a32:12_3": "purisapuriseti attho.",
  "atk-s0402a32:12_4": "Imasmiṁ sutte tīṇi maggaphalāni kathitāni.",
  "atk-s0402a32:12_5": "Tattha ayaṁ tīhi maggehi ñāṇajavasampannoti veditabbo.",
  "atk-s0402a32:13_0": "10. Assājānīyasuttavaṇṇanā",
  "atk-s0402a32:14_0": "143.",
  "atk-s0402a32:14_1": "Dasame bhadreti bhaddake.",
  "atk-s0402a32:14_2": "Assājānīyeti kāraṇākāraṇaṁ jānanake asse.",
  "atk-s0402a32:14_3": "Purisājānīyesupi eseva nayo.",
  "atk-s0402a32:14_4": "Imasmiṁ sutte arahattaphalaṁ kathitaṁ.",
  "atk-s0402a32:14_5": "Tatrāyaṁ arahattamaggena ñāṇajavasampannoti veditabbo.",
  "atk-s0402a32:15_0": "11. Paṭhamamoranivāpasuttavaṇṇanā",
  "atk-s0402a32:16_0": "144.",
  "atk-s0402a32:16_1": "Ekādasame accantaniṭṭhoti antaṁ atikkantaniṭṭho,",
  "atk-s0402a32:16_2": "akuppaniṭṭho dhuvaniṭṭhoti attho.",
  "atk-s0402a32:16_3": "Sesaṁ sadisameva.",
  "atk-s0402a32:17_0": "12. Dutiyamoranivāpasuttavaṇṇanā",
  "atk-s0402a32:18_0": "145.",
  "atk-s0402a32:18_1": "Dvādasame iddhipāṭihāriyenāti ijjhanakapāṭihāriyena.",
  "atk-s0402a32:18_2": "Ādesanāpāṭihāriyenāti ādisitvā apadisitvā kathanaanukathanakathāpāṭihāriyena.",
  "atk-s0402a32:19_0": "13. Tatiyamoranivāpasuttavaṇṇanā",
  "atk-s0402a32:20_0": "146.",
  "atk-s0402a32:20_1": "Terasame sammādiṭṭhiyāti phalasamāpattatthāya sammādiṭṭhiyā.",
  "atk-s0402a32:20_2": "Sammāñāṇenāti phalañāṇena.",
  "atk-s0402a32:20_3": "Sammāvimuttiyāti sesehi phalasamāpattidhammehi.",
  "atk-s0402a32:20_4": "Imesu tīsupi suttesu khīṇāsavova kathitoti.",
  "atk-s0402a32:21_0": "Yodhājīvavaggo catuttho."
}