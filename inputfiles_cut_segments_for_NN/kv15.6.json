{
  "kv15.6:0.1_0": "Kathāvatthu",
  "kv15.6:0.2_0": "Mahāpaṇṇāsaka",
  "kv15.6:0.3_0": "Pannarasamavagga",
  "kv15.6:0.4_0": "Jarāmaraṇakathā",
  "kv15.6:1.1_0": "Lokuttarānaṁ dhammānaṁ jarāmaraṇaṁ lokuttaranti?",
  "kv15.6:1.2_0": "Āmantā.",
  "kv15.6:1.3_0": "Maggo phalaṁ nibbānaṁ, sotāpattimaggo sotāpattiphalaṁ …pe… bojjhaṅgoti?",
  "kv15.6:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv15.6:1.5_0": "sotāpattimaggassa jarāmaraṇaṁ sotāpattimaggoti?",
  "kv15.6:1.6_0": "Na hevaṁ vattabbe …pe…",
  "kv15.6:1.7_0": "sotāpattimaggassa jarāmaraṇaṁ sotāpattimaggoti?",
  "kv15.6:1.8_0": "Āmantā.",
  "kv15.6:1.9_0": "Sotāpattiphalassa jarāmaraṇaṁ sotāpattiphalanti?",
  "kv15.6:1.10_0": "Na hevaṁ vattabbe …pe…",
  "kv15.6:1.11_0": "sakadāgāmimaggassa …pe…",
  "kv15.6:1.12_0": "sakadāgāmiphalassa …pe…",
  "kv15.6:1.13_0": "anāgāmimaggassa …pe…",
  "kv15.6:1.14_0": "anāgāmiphalassa …pe…",
  "kv15.6:1.15_0": "arahattamaggassa jarāmaraṇaṁ arahattamaggoti?",
  "kv15.6:1.16_0": "Na hevaṁ vattabbe …pe…",
  "kv15.6:1.17_0": "arahattamaggassa jarāmaraṇaṁ arahattamaggoti?",
  "kv15.6:1.18_0": "Āmantā.",
  "kv15.6:1.19_0": "Arahattaphalassa jarāmaraṇaṁ arahattaphalanti?",
  "kv15.6:1.20_0": "Na hevaṁ vattabbe …pe…",
  "kv15.6:1.21_0": "satipaṭṭhānānaṁ …",
  "kv15.6:1.22_0": "sammappadhānānaṁ …",
  "kv15.6:1.23_0": "iddhipādānaṁ …",
  "kv15.6:1.24_0": "indriyānaṁ …",
  "kv15.6:1.25_0": "balānaṁ …",
  "kv15.6:1.26_0": "bojjhaṅgānaṁ jarāmaraṇaṁ bojjhaṅgoti?",
  "kv15.6:1.27_0": "Na hevaṁ vattabbe …pe….",
  "kv15.6:2.1_0": "Na vattabbaṁ—",
  "kv15.6:2.2_0": "“lokuttarānaṁ dhammānaṁ jarāmaraṇaṁ lokuttaran”ti?",
  "kv15.6:2.3_0": "Āmantā.",
  "kv15.6:2.4_0": "Lokiyanti?",
  "kv15.6:2.5_0": "Na hevaṁ vattabbe.",
  "kv15.6:2.6_0": "Tena hi lokuttaranti.",
  "kv15.6:2.7_0": "Jarāmaraṇakathā niṭṭhitā."
}