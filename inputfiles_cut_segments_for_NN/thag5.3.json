{
  "thag5.3:0.1_0": "Theragāthā",
  "thag5.3:0.2_0": "Pañcakanipāta",
  "thag5.3:0.3_0": "Paṭhamavagga",
  "thag5.3:0.4_0": "5.3. Girimānandattheragāthā",
  "thag5.3:1.1_0": "“Vassati devo yathā sugītaṁ,",
  "thag5.3:1.2_0": "Channā me kuṭikā sukhā nivātā;",
  "thag5.3:1.3_0": "Tassaṁ viharāmi vūpasanto,",
  "thag5.3:1.4_0": "Atha ce patthayasī pavassa deva.",
  "thag5.3:2.1_0": "Vassati devo yathā sugītaṁ,",
  "thag5.3:2.2_0": "Channā me kuṭikā sukhā nivātā;",
  "thag5.3:2.3_0": "Tassaṁ viharāmi santacitto,",
  "thag5.3:2.4_0": "Atha ce patthayasī pavassa deva.",
  "thag5.3:3.1_0": "Vassati devo,",
  "thag5.3:3.2_0": "…",
  "thag5.3:3.3_0": "pe…",
  "thag5.3:3.4_0": "tassaṁ viharāmi vītarāgo,",
  "thag5.3:3.5_0": "…",
  "thag5.3:3.6_0": "pe…",
  "thag5.3:4.1_0": "Vassati devo,",
  "thag5.3:4.2_0": "…",
  "thag5.3:4.3_0": "pe…",
  "thag5.3:4.4_0": "tassaṁ viharāmi vītadoso,",
  "thag5.3:4.5_0": "…",
  "thag5.3:4.6_0": "pe…",
  "thag5.3:5.1_0": "Vassati devo,",
  "thag5.3:5.2_0": "…",
  "thag5.3:5.3_0": "pe…",
  "thag5.3:5.4_0": "tassaṁ viharāmi vītamoho,",
  "thag5.3:5.5_0": "Atha ce patthayasī pavassa devā”ti.",
  "thag5.3:5.6_0": "… Girimānando thero …."
}