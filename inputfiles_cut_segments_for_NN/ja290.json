{
  "ja290:0.1_0": "Jātaka",
  "ja290:0.2_0": "Tikanipāta",
  "ja290:0.3_0": "Abbhantaravagga",
  "ja290:0.4_0": "10. Sīlavīmaṁsakajātaka",
  "ja290:1.1_0": "“Sīlaṁ kireva kalyāṇaṁ,",
  "ja290:1.2_0": "sīlaṁ loke anuttaraṁ;",
  "ja290:1.3_0": "Passa ghoraviso nāgo,",
  "ja290:1.4_0": "sīlavāti na haññati.",
  "ja290:2.1_0": "Sohaṁ sīlaṁ samādissaṁ,",
  "ja290:2.2_0": "loke anumataṁ sivaṁ;",
  "ja290:2.3_0": "Ariyavuttisamācāro,",
  "ja290:2.4_0": "yena vuccati sīlavā.",
  "ja290:3.1_0": "Ñātīnañca piyo hoti,",
  "ja290:3.2_0": "mittesu ca virocati;",
  "ja290:3.3_0": "Kāyassa bhedā sugatiṁ,",
  "ja290:3.4_0": "upapajjati sīlavā”ti.",
  "ja290:4.1_0": "Sīlavīmaṁsakajātakaṁ dasamaṁ.",
  "ja290:4.2_0": "Abbhantaravaggo catuttho.",
  "ja290:5.1_0": "Tassuddānaṁ",
  "ja290:6.1_0": "Duma kaṁsavaruttamabyagghamigā,",
  "ja290:6.2_0": "Maṇayo maṇi sālukamavhayano;",
  "ja290:6.3_0": "Anusāsaniyopi ca macchavaro,",
  "ja290:6.4_0": "Maṇikuṇḍalakena kirena dasāti."
}