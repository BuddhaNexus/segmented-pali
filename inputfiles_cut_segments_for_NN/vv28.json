{
  "vv28:0.1_0": "Vimānavatthu",
  "vv28:0.2_0": "Itthivimāna",
  "vv28:0.3_0": "Cittalatāvagga",
  "vv28:0.4_0": "11. Dutiyabhikkhādāyikāvimānavatthu",
  "vv28:1.1_0": "“Abhikkantena vaṇṇena,",
  "vv28:1.2_0": "yā tvaṁ tiṭṭhasi devate;",
  "vv28:1.3_0": "Obhāsentī disā sabbā,",
  "vv28:1.4_0": "osadhī viya tārakā.",
  "vv28:2.1_0": "Kena tetādiso vaṇṇo,",
  "vv28:2.2_0": "…pe…",
  "vv28:2.3_0": "vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv28:3.1_0": "Sā devatā attamanā,",
  "vv28:3.2_0": "…pe…",
  "vv28:3.3_0": "yassa kammassidaṁ phalaṁ.",
  "vv28:4.1_0": "“Ahaṁ manussesu manussabhūtā,",
  "vv28:4.2_0": "Purimāya jātiyā manussaloke.",
  "vv28:5.1_0": "Addasaṁ virajaṁ bhikkhuṁ,",
  "vv28:5.2_0": "vippasannamanāvilaṁ;",
  "vv28:5.3_0": "Tassa adāsahaṁ bhikkhaṁ,",
  "vv28:5.4_0": "pasannā sehi pāṇibhi.",
  "vv28:6.1_0": "Tena metādiso vaṇṇo,",
  "vv28:6.2_0": "…pe…",
  "vv28:6.3_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv28:7.1_0": "Dutiyabhikkhādāyikāvimānaṁ ekādasamaṁ.",
  "vv28:8.1_0": "Tassuddānaṁ",
  "vv28:9.1_0": "Dāsī ceva lakhumā ca,",
  "vv28:9.2_0": "atha ācāmadāyikā;",
  "vv28:9.3_0": "Caṇḍālī bhadditthī ceva,",
  "vv28:9.4_0": "soṇadinnā uposathā;",
  "vv28:9.5_0": "Niddā ceva suniddā ca,",
  "vv28:9.6_0": "dve ca bhikkhāya dāyikā;",
  "vv28:9.7_0": "Vaggo tena pavuccatīti.",
  "vv28:10.1_0": "Itthivimāne dutiyo vaggo.",
  "vv28:11.1_0": "Bhāṇavāraṁ paṭhamaṁ."
}