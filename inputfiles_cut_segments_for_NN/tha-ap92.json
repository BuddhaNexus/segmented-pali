{
  "tha-ap92:0.1_0": "Therāpadāna",
  "tha-ap92:0.2_0": "Timiravagga",
  "tha-ap92:0.3_0": "10. Padumapupphiyattheraapadāna",
  "tha-ap92:1.1_0": "“Pokkharavanaṁ paviṭṭho,",
  "tha-ap92:1.2_0": "Bhañjanto padumānihaṁ;",
  "tha-ap92:1.3_0": "Tatthaddasaṁ phussaṁ buddhaṁ,",
  "tha-ap92:1.4_0": "Bāttiṁsavaralakkhaṇaṁ.",
  "tha-ap92:2.1_0": "Padumapupphaṁ gahetvāna,",
  "tha-ap92:2.2_0": "ākāse ukkhipiṁ ahaṁ;",
  "tha-ap92:2.3_0": "Pāpakammaṁ saritvāna,",
  "tha-ap92:2.4_0": "pabbajiṁ anagāriyaṁ.",
  "tha-ap92:3.1_0": "Pabbajitvāna kāyena,",
  "tha-ap92:3.2_0": "manasā saṁvutena ca;",
  "tha-ap92:3.3_0": "Vacīduccaritaṁ hitvā,",
  "tha-ap92:3.4_0": "ājīvaṁ parisodhayiṁ.",
  "tha-ap92:4.1_0": "Dvenavute ito kappe,",
  "tha-ap92:4.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap92:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap92:4.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap92:5.1_0": "Padumābhāsanāmā ca,",
  "tha-ap92:5.2_0": "aṭṭhārasa mahīpatī;",
  "tha-ap92:5.3_0": "Aṭṭhārasesu kappesu,",
  "tha-ap92:5.4_0": "aṭṭhatālīsamāsisuṁ.",
  "tha-ap92:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap92:6.2_0": "…pe…",
  "tha-ap92:6.3_0": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap92:7.1_0": "Itthaṁ sudaṁ āyasmā padumapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap92:8.1_0": "Padumapupphiyattherassāpadānaṁ dasamaṁ.",
  "tha-ap92:9.1_0": "Timiravaggo navamo.",
  "tha-ap92:10.1_0": "Tassuddānaṁ",
  "tha-ap92:11.1_0": "Timiranaṅgalīpuppha,",
  "tha-ap92:11.2_0": "nippannañjaliko adho;",
  "tha-ap92:11.3_0": "Dve raṁsisaññī phalado,",
  "tha-ap92:11.4_0": "saddasaññī ca secako;",
  "tha-ap92:11.5_0": "Padmapupphī ca gāthāyo,",
  "tha-ap92:11.6_0": "chappaññāsa pakittitā."
}