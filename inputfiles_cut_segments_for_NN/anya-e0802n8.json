{
  "anya-e0802n8:0_0": "8. Uṇādikappa",
  "anya-e0802n8:1_0": "Chaṭṭhakaṇḍa",
  "anya-e0802n8:2_0": "624, 563.Kattarikita.",
  "anya-e0802n8:3_0": "Kattuiccetasmiṁ atthe kita paccayā honti.",
  "anya-e0802n8:4_0": "Kāru, kāruko, kārako, pācako, kattā, janitā, pacitā, netā.",
  "anya-e0802n8:5_0": "625, 605.Bhāvakammesu kiccatta khatthā.",
  "anya-e0802n8:6_0": "Bhāvakammaiccetesvatthesu kicca tta khatthaiccete paccayā honti.",
  "anya-e0802n8:7_0": "Upasampādetabbaṁ upasampādanīyaṁ bhavatā,",
  "anya-e0802n8:7_1": "sayitabbaṁ bhavatā,",
  "anya-e0802n8:7_2": "kattabbaṁ bhavatā,",
  "anya-e0802n8:7_3": "bhottabbo odano bhavatā,",
  "anya-e0802n8:7_4": "asitabbaṁ bhojanaṁ bhavatā,",
  "anya-e0802n8:7_5": "asitaṁ bhavatā,",
  "anya-e0802n8:7_6": "sayitaṁ bhavatā,",
  "anya-e0802n8:7_7": "pacitaṁ bhavatā,",
  "anya-e0802n8:7_8": "asitaṁ asanaṁ bhavatā,",
  "anya-e0802n8:7_9": "sayitaṁ sayanaṁ bhavatā,",
  "anya-e0802n8:7_10": "pacito odano bhavatā,",
  "anya-e0802n8:7_11": "kiñcissayo, īsassayo, dussayo,",
  "anya-e0802n8:7_12": "sussayo bhavatā.",
  "anya-e0802n8:8_0": "626, 634.Kammanidutiyāya tto.",
  "anya-e0802n8:9_0": "Kammani iccetasmiṁ atthe dutiyāyaṁ vibhattiyaṁ kattari ttapaccayo hoti.",
  "anya-e0802n8:10_0": "Dānaṁ dinno devadatto, sīlaṁ rakkhito devadatto, bhattaṁ bhutto devadatto, garuṁ upāsito devadatto.",
  "anya-e0802n8:11_0": "627, 652.Khyādīhi māna ma ca to vā.",
  "anya-e0802n8:12_0": "Khi bhī su ru hu vā dhū hi lū pī adaiccevamādīhi dhātūhi mana paccayo hoti, massa ca to hoti vā.",
  "anya-e0802n8:13_0": "Khīyanti upaddavā etthāti khemo,",
  "anya-e0802n8:13_1": "bhāyitabboti bhemo,",
  "anya-e0802n8:13_2": "bhāyanti etasmāti vā bhemo,",
  "anya-e0802n8:13_3": "raṁsiyo abhissavetīti somo,",
  "anya-e0802n8:13_4": "ravati gacchatīti romo,",
  "anya-e0802n8:13_5": "huvati juhvati etenāti homo,",
  "anya-e0802n8:13_6": "paṭilomavasena vāti gacchatīti vāmo,",
  "anya-e0802n8:13_7": "lāmakavasena vāti gacchati pavattatīti vā vāmo,",
  "anya-e0802n8:13_8": "dhunāti kampatīti dhūmo,",
  "anya-e0802n8:13_9": "seṭṭhabhāvena hinoti pavattati cittaṁ etasminti hemo,",
  "anya-e0802n8:13_10": "lunitabboti lomo,",
  "anya-e0802n8:13_11": "maṁsacammāni lunāti chindatīti vā lomo,",
  "anya-e0802n8:13_12": "piyanaṁ pemo,",
  "anya-e0802n8:13_13": "piyāyitabboti vā pemo,",
  "anya-e0802n8:13_14": "sukhadukkhaṁ adati bhakkhatīti attā,",
  "anya-e0802n8:13_15": "jātijarāmaraṇādīhi adīyate bhakkhīyateti vā attā, ātumā.",
  "anya-e0802n8:14_0": "628, 653.Samādīhitha mā.",
  "anya-e0802n8:15_0": "Samu damu dara raha du hi si bhī dā yā sā ṭhābhasaiccevamādīhi dhātūhi tha ma paccayā honti.",
  "anya-e0802n8:16_0": "Sametīti samatho,",
  "anya-e0802n8:16_1": "damatīti damatho,",
  "anya-e0802n8:16_2": "damanaṁ vā damatho,",
  "anya-e0802n8:16_3": "damitabboti vā damatho,",
  "anya-e0802n8:16_4": "daratīti daratho,",
  "anya-e0802n8:16_5": "jiṇṇabhāvaṁ rahissati gaṇhissatīti ratho,",
  "anya-e0802n8:16_6": "dabbasambhāre rahati gaṇhātīti vā ratho,",
  "anya-e0802n8:16_7": "davati gacchatīti dumo,",
  "anya-e0802n8:16_8": "davati vuddhi viruḷhi gacchati pavattatīti uddhaṁ vā dumo,",
  "anya-e0802n8:16_9": "pathavīpabbatādīsu gacchati patatīti himo,",
  "anya-e0802n8:16_10": "kammavācāya bandhati etthāti sīmā.",
  "anya-e0802n8:16_11": "Bandhitabbāti vā sīmā.",
  "anya-e0802n8:16_12": "Bhāyanti etasmāti bhīmo,",
  "anya-e0802n8:16_13": "satte avakhaṇḍenti nivārenti etenāti dāmo,",
  "anya-e0802n8:16_14": "mūsikādīhi khādīyati avakhaṇḍīyatīti vā dāmo,",
  "anya-e0802n8:16_15": "yāti gacchatīti yāmo,",
  "anya-e0802n8:16_16": "paresaṁ cittaṁ gaṇhituṁ samatthetīti sāmo,",
  "anya-e0802n8:16_17": "tiṭṭhanti etenāti thāmo,",
  "anya-e0802n8:16_18": "bhasati bhasmīkarīyatīti bhasmā.",
  "anya-e0802n8:17_0": "629, 569.Gahassu’padhasse vā.",
  "anya-e0802n8:18_0": "Gahaiccetassa dhātussa upadhassa akārassa etta hoti vā.",
  "anya-e0802n8:19_0": "Dabbasambhāra gaṇhātīti gehaṁ, gahaṁ.",
  "anya-e0802n8:20_0": "630, 654.Masussasussa cchara ccherā.",
  "anya-e0802n8:21_0": "Masuiccetassa pāṭipadikassa sussa ccharaccherādesā honti.",
  "anya-e0802n8:22_0": "Maccharatīti maccharo, evaṁ macchero.",
  "anya-e0802n8:23_0": "631, 655.Āpubbacarassa ca.",
  "anya-e0802n8:24_0": "Āpubbassa caraiccetassa dhātussa cchariyaccharaccherā desā honti, āpubbassa ca rasso hoti.",
  "anya-e0802n8:25_0": "Ābhuso caritabbanti acchariyaṁ. Evaṁ accharaṁ, accheraṁ.",
  "anya-e0802n8:26_0": "Caggahaṇena masussa sussāpi cchariyādeso hoti, macchariyaṁ.",
  "anya-e0802n8:27_0": "632, 656.Ala kala salehi layā.",
  "anya-e0802n8:28_0": "Ala kala salaiccetehi dhātūhi la yapaccayā honti.",
  "anya-e0802n8:29_0": "Alati samatthetīti allaṁ,",
  "anya-e0802n8:29_1": "kalitabbaṁ saṅkhyātabbanti kallaṁ,",
  "anya-e0802n8:29_2": "salati gacchati pavisatīti sallaṁ.",
  "anya-e0802n8:29_3": "Evaṁ alyaṁ, kalyaṁ, salyaṁ.",
  "anya-e0802n8:30_0": "633, 657.Yāṇalāṇā.",
  "anya-e0802n8:31_0": "Tehi kala salaiccetehi dhātūhi yāṇa lāṇapaccayā honti.",
  "anya-e0802n8:32_0": "Kalitabbaṁ saṅkhyātabbanti kalyāṇaṁ,",
  "anya-e0802n8:32_1": "gaṇato paṭikkamitvā salanti etthāti paṭisalyāṇaṁ.",
  "anya-e0802n8:32_2": "Evaṁ sallāṇo,",
  "anya-e0802n8:32_3": "paṭisallāṇo.",
  "anya-e0802n8:33_0": "634, 658.Mathissa thassa lo ca.",
  "anya-e0802n8:34_0": "Mathaiccetassa dhātussa thassa lādesohoti. Aññamaññaṁ mathati viloḷatīti mallo, mallaṁ.",
  "anya-e0802n8:35_0": "Caggahaṇena lato ko ca āgamo hoti. Mallako, mallakaṁ.",
  "anya-e0802n8:36_0": "635, 559.Pesātisaggapattakālesukiccā.",
  "anya-e0802n8:37_0": "Pesa atisagga pattakālaiccetesvatthesu kiccapaccayā honti.",
  "anya-e0802n8:38_0": "Kattabbaṁ kammaṁ bhavatā,",
  "anya-e0802n8:38_1": "karaṇīyaṁ kiccaṁ bhavatā,",
  "anya-e0802n8:38_2": "bhottabbaṁ bhojanaṁ bhavatā,",
  "anya-e0802n8:38_3": "bhojanīyaṁ bhojanaṁ bhavatā,",
  "anya-e0802n8:38_4": "ajjhayitabbaṁ ajjheyyaṁ bhavatā,",
  "anya-e0802n8:38_5": "ajjhayanīyaṁ ajjheyyaṁ bhavatā.",
  "anya-e0802n8:39_0": "636, 659.Avassakā’dhamiṇesuṇī ca.",
  "anya-e0802n8:40_0": "Avassaka adhamiṇaiccetesvatthesu ṇīpaccayo hoti, kiccā ca.",
  "anya-e0802n8:41_0": "Avassate tāva – kārīsi me kammaṁ avassaṁ hārīsi me bhāraṁ avassaṁ.",
  "anya-e0802n8:42_0": "Adhamiṇe – dāyīsi me sataṁ iṇaṁ, dhārīsi me sahassaṁ iṇaṁ.",
  "anya-e0802n8:43_0": "Kiccā ca –",
  "anya-e0802n8:43_1": "dātabbaṁ me bhavatā sataṁ iṇaṁ.",
  "anya-e0802n8:43_2": "Dhārayitabbaṁ me bhavatā sahassaṁ iṇaṁ,",
  "anya-e0802n8:43_3": "kattabbaṁ me bhavatā gehaṁ,",
  "anya-e0802n8:43_4": "karaṇīyaṁ me bhavatā kiccaṁ,",
  "anya-e0802n8:43_5": "kāriyaṁ me bhavatā sayanaṁ.",
  "anya-e0802n8:44_0": "637, 0.Arahasakkādīhi tuṁ.",
  "anya-e0802n8:45_0": "Araha sakka bhabbaiccevamādīhi payoge sati sabbadhātūhi tuṁpaccayo hoti.",
  "anya-e0802n8:46_0": "Arahā bhavaṁ vattuṁ,",
  "anya-e0802n8:46_1": "arahā bhavaṁ kattuṁ,",
  "anya-e0802n8:46_2": "sakkā bhavaṁ hantuṁ,",
  "anya-e0802n8:46_3": "sattā bhavaṁ janetuṁ, janituṁ, bhavituṁ,",
  "anya-e0802n8:46_4": "sakkā bhavaṁ dātuṁ,",
  "anya-e0802n8:46_5": "sakkā bhavaṁ gantuṁ,",
  "anya-e0802n8:46_6": "tabbo bhavaṁ janetuṁ iccevamādi.",
  "anya-e0802n8:47_0": "638, 660.Vajādīhipabbajjādayonippajjante.",
  "anya-e0802n8:48_0": "Vajaiccevamādīhi dhātūhi, upasaggapaccayādīhi ca pabbajjādayo saddā nippajjante.",
  "anya-e0802n8:49_0": "Paṭhamameva vajitabbāti pabbajjā,",
  "anya-e0802n8:49_1": "iñjanaṁ ejjā,",
  "anya-e0802n8:49_2": "samajjanaṁ samajjā,",
  "anya-e0802n8:49_3": "nisīdanaṁ nisajjā,",
  "anya-e0802n8:49_4": "vijānanaṁ vijjā visajjanaṁ visajjā,",
  "anya-e0802n8:49_5": "padanaṁ pajjā,",
  "anya-e0802n8:49_6": "hananaṁ vajjhā,",
  "anya-e0802n8:49_7": "esanaṁ icchā,",
  "anya-e0802n8:49_8": "atiesanaṁ aticchā,",
  "anya-e0802n8:49_9": "sadanaṁ sajjā,",
  "anya-e0802n8:49_10": "sayanti etthāti seyyā,",
  "anya-e0802n8:49_11": "sammā cittaṁ nidheti etāyāti saddhā,",
  "anya-e0802n8:49_12": "caritabbā cariyā,",
  "anya-e0802n8:49_13": "karaṇaṁ kiriyā,",
  "anya-e0802n8:49_14": "rujanaṁ rucchā,",
  "anya-e0802n8:49_15": "padanaṁ pacchā,",
  "anya-e0802n8:49_16": "riñcanaṁ ricchā,",
  "anya-e0802n8:49_17": "tikicchanaṁ titicchā,",
  "anya-e0802n8:49_18": "saṁkocanaṁ saṁkucchā,",
  "anya-e0802n8:49_19": "madanaṁ macchā,",
  "anya-e0802n8:49_20": "labhanaṁ lacchā,",
  "anya-e0802n8:49_21": "radihabbāti racchā,",
  "anya-e0802n8:49_22": "radanaṁ vilekhanaṁ vāracchā,",
  "anya-e0802n8:49_23": "adho bhāgena gacchatīti tiracchā,",
  "anya-e0802n8:49_24": "tiracchāno,",
  "anya-e0802n8:49_25": "ajanaṁ acchā,",
  "anya-e0802n8:49_26": "titikkhatīti titikkhā,",
  "anya-e0802n8:49_27": "saha āgamanaṁ sāgacchā,",
  "anya-e0802n8:49_28": "duṭṭhu bhakkhanaṁ dobhacchā,",
  "anya-e0802n8:49_29": "duṭṭhu rosanaṁ durucchā,",
  "anya-e0802n8:49_30": "pucchanaṁ pucchā,",
  "anya-e0802n8:49_31": "muhanaṁ mucchā,",
  "anya-e0802n8:49_32": "vasanaṁ vacchā,",
  "anya-e0802n8:49_33": "kacanaṁ kacchā,",
  "anya-e0802n8:49_34": "saha kathanaṁ sākacchā,",
  "anya-e0802n8:49_35": "tudanaṁ tucchā,",
  "anya-e0802n8:49_36": "visanaṁ vicchā,",
  "anya-e0802n8:49_37": "pisanaṁ picchillā,",
  "anya-e0802n8:49_38": "sukhadukkhaṁ mudati bhakkhatīti maccho ,",
  "anya-e0802n8:49_39": "sattānaṁ pāṇaṁ museti cajetīti maccu,",
  "anya-e0802n8:49_40": "satanaṁ saccaṁ,",
  "anya-e0802n8:49_41": "uddhaṁ dhunāti kampatīti uddhaccaṁ,",
  "anya-e0802n8:49_42": "naṭanaṁ naccaṁ,",
  "anya-e0802n8:49_43": "nitanaṁ niccaṁ,",
  "anya-e0802n8:49_44": "tathanaṁ tacchaṁ iccevamādi.",
  "anya-e0802n8:50_0": "639, 585.Kvilopo ca.",
  "anya-e0802n8:51_0": "Kvilopo hoti, puna ca nippajjante.",
  "anya-e0802n8:52_0": "Vividhehi sīlādiguṇehi bhavatīti vibhū,",
  "anya-e0802n8:52_1": "visesena vā bhavatīti vibhū,",
  "anya-e0802n8:52_2": "sayaṁ attanā bhavatīti sayambhū,",
  "anya-e0802n8:52_3": "abhivitvā bhavatīti abhibhū,",
  "anya-e0802n8:52_4": "saṁ suṭṭhu dhunāti kampatīti sandhū,",
  "anya-e0802n8:52_5": "visesena bhāti dibbatīti vibhā,",
  "anya-e0802n8:52_6": "nissesena bhāti dibbatīti nibhā,",
  "anya-e0802n8:52_7": "pakārena bhāti dibbatīti pabhā,",
  "anya-e0802n8:52_8": "saha bhāsanti etthāti sabhā,",
  "anya-e0802n8:52_9": "ābhuso bhāti dibbatīti ābhā,",
  "anya-e0802n8:52_10": "bhujena kuṭilena gacchatīti bhujago,",
  "anya-e0802n8:52_11": "turitaturito gacchatīti turago,",
  "anya-e0802n8:52_12": "saṁ suṭṭhu pathaviṁ khanatīti saṅkho,",
  "anya-e0802n8:52_13": "visesena yamati uparamatīti viyo,",
  "anya-e0802n8:52_14": "suṭṭhu manati jānātīti sumo,",
  "anya-e0802n8:52_15": "pari samantato tanoti vitthāretīti parito iccevamādi.",
  "anya-e0802n8:53_0": "640, 0.Sacajānaṁ kagā ṇānubandhe.",
  "anya-e0802n8:54_0": "Saca jānaṁ dhātūnamantānaṁ cajānaṁ kagādesā honti yathāsaṅkhyaṁ ṇānubandhe paccaye pare.",
  "anya-e0802n8:55_0": "Oko , pāko, seko, soko, viveko, cāgo, yogo, bhogo, rogo, rāgo, bhāgo, bhaṅgo, raṅgo, saṅgo.",
  "anya-e0802n8:56_0": "641, 572.Nudādīhi yuṇvūnamanānanākānanakā sakāritehi ca.",
  "anya-e0802n8:57_0": "Nuda sūda jana su lū hu pu bhū ñā a sa samuiccevamādīhi dhātūhi,",
  "anya-e0802n8:57_1": "phanda citi āṇa iccevamādīhi sakāritehi ca yuṇvūnaṁ paccayānaṁ ana ānana aka ānanakādesā honti yathāsaṅkhyaṁ kattari,",
  "anya-e0802n8:57_2": "bhāvakaraṇesu ca.",
  "anya-e0802n8:58_0": "Kattari tāva –",
  "anya-e0802n8:58_1": "panudatīti panūdano.",
  "anya-e0802n8:58_2": "Evaṁ sūdano, janano, savaṇo, lavano, havano, pavano, bhavano, ñāṇo, asano, samaṇo.",
  "anya-e0802n8:59_0": "Bhāve ca –",
  "anya-e0802n8:59_1": "panudate panūdanaṁ.",
  "anya-e0802n8:59_2": "Evaṁ sūdanaṁ, jananaṁ, savaṇaṁ, lavanaṁ, havanaṁ, pavanaṁ, bhavanaṁ, ñāṇaṁ, asanaṁ, samaṇaṁ,",
  "anya-e0802n8:59_3": "sañjānanaṁ,",
  "anya-e0802n8:59_4": "kuyate kānanaṁ.",
  "anya-e0802n8:60_0": "Kārite ca – phandāpīyate phandāpanaṁ, cetāpīyate cetāpanaṁ, āṇāpīyate āṇāpanaṁ.",
  "anya-e0802n8:61_0": "Karaṇe –",
  "anya-e0802n8:61_1": "nudanti anenāti nūdanaṁ,",
  "anya-e0802n8:61_2": "evaṁ sūdanaṁ, jananaṁ, savaṇaṁ, lavaṇaṁ, havanaṁ, pavanaṁ, bhaganaṁ, ñāṇaṁ, asanaṁ, savaṇaṁ.",
  "anya-e0802n8:62_0": "Puna kattari –",
  "anya-e0802n8:62_1": "nudatīti nūdako,",
  "anya-e0802n8:62_2": "sūdatīti sūdadhakā,",
  "anya-e0802n8:62_3": "janetīti janako,",
  "anya-e0802n8:62_4": "suṇotīti sāvako,",
  "anya-e0802n8:62_5": "lunātīti lāvako,",
  "anya-e0802n8:62_6": "juhotīti hāvako,",
  "anya-e0802n8:62_7": "punātīti pāvako,",
  "anya-e0802n8:62_8": "bhavatīti bhāvako,",
  "anya-e0802n8:62_9": "jānātīti jānako,",
  "anya-e0802n8:62_10": "asatīti asako,",
  "anya-e0802n8:62_11": "upāsatīti upāsako,",
  "anya-e0802n8:62_12": "sametīti samako.",
  "anya-e0802n8:63_0": "Kārite tu – phandāpayatīti phandāpayako. Evaṁ āṇāpayako, cetāpayako, sañjānanako.",
  "anya-e0802n8:64_0": "642, 588.I ya ta ma ki e sāna’mantassaro dīghaṁ kvaci du sa ssa guṇaṁ do raṁ sakkhī ca.",
  "anya-e0802n8:65_0": "Iya ta ma ki e saiccetesaṁ sabbanāmānamanto saro dīghamāpajjate,",
  "anya-e0802n8:65_1": "kvaci dusaiccetassa dhātussa ukāro guṇamāpajjate,",
  "anya-e0802n8:65_2": "dakāro rakāramāpajjate,",
  "anya-e0802n8:65_3": "dhātvantassa sassa ca sa kkha īiccete ādesā honti yathāsambhavaṁ.",
  "anya-e0802n8:65_4": "Ete saddā sakena sakena nāmena yathānuparodhena buddhasāsane pacchā puna nippajjante.",
  "anya-e0802n8:66_0": "Imamiva naṁ passatīti īdiso,",
  "anya-e0802n8:66_1": "yamiva naṁ passatīti yādiso,",
  "anya-e0802n8:66_2": "tamiva naṁ passatīti tādiso,",
  "anya-e0802n8:66_3": "mamiva naṁ passatīti mādiso,",
  "anya-e0802n8:66_4": "kimiva naṁ passatīti kīdiso,",
  "anya-e0802n8:66_5": "etamiva naṁ passatīti ediso,",
  "anya-e0802n8:66_6": "samānamiva naṁ passatīti sādiso .",
  "anya-e0802n8:66_7": "Imamiva naṁ passatīti īriso,",
  "anya-e0802n8:66_8": "yamiva naṁ passatīti yāriso,",
  "anya-e0802n8:66_9": "tamiva naṁ passatīti tāriso,",
  "anya-e0802n8:66_10": "mamiva naṁ passatīti māriso,",
  "anya-e0802n8:66_11": "kimiva naṁ passatīti kīriso,",
  "anya-e0802n8:66_12": "etamiva naṁ passatīti eriso,",
  "anya-e0802n8:66_13": "samānamiva naṁ passatīti sāriso.",
  "anya-e0802n8:66_14": "Imamiva naṁ passatīti īdikkho,",
  "anya-e0802n8:66_15": "yamiva naṁ passatīti yādikkho,",
  "anya-e0802n8:66_16": "tamiva naṁ passatīti tādikkho,",
  "anya-e0802n8:66_17": "evaṁ mādikkho, kīdikkho, edikkho, sādikkho. Īdī, yādī, tādī, mādī, kīdī, edī, sādī.",
  "anya-e0802n8:67_0": "Caggahaṇena tesameva saddānaṁ iyaiccevamādīnamanto ca saro kvaci dīghatthamāhu. Īdikkho, yādikkho, tādikkho, mādikkho, kīdikkho, edikkho, sādikkho. Idiso, sadiso, sariso, sarikkho.",
  "anya-e0802n8:68_0": "643, 635.Bhyādīhi mati budhi pūjādīhi ca tto.",
  "anya-e0802n8:69_0": "Bhīiccevamādīhi dhātūhi, mati. Budhi pūjādito ca tta paccayo hoti.",
  "anya-e0802n8:70_0": "Bhāyitabboti bhīto,",
  "anya-e0802n8:70_1": "supitabboti sutto,",
  "anya-e0802n8:70_2": "mijjitabbo sinehetabboti mitto,",
  "anya-e0802n8:70_3": "sammannitabboti sammato,",
  "anya-e0802n8:70_4": "saṁ suṭṭhu mānitabbo pūjetabboti sammato,",
  "anya-e0802n8:70_5": "sammānīyitthāti sammato,",
  "anya-e0802n8:70_6": "saṁkappīyateti saṅkappito ,",
  "anya-e0802n8:70_7": "saṁkappīyitthāti saṅkappito,",
  "anya-e0802n8:70_8": "sampādīyateti sampādito,",
  "anya-e0802n8:70_9": "sampādīyitthāti sampādito,",
  "anya-e0802n8:70_10": "avadhārīyateti avadhārito,",
  "anya-e0802n8:70_11": "avadhārīyitthāti avadhārito,",
  "anya-e0802n8:70_12": "bujjhitabbo ñātabboti buddho,",
  "anya-e0802n8:70_13": "ajjhayitabboti ito,",
  "anya-e0802n8:70_14": "etabbo gantabboti ito,",
  "anya-e0802n8:70_15": "viditabbo ñātabboti vidito,",
  "anya-e0802n8:70_16": "takkīyateti takkito,",
  "anya-e0802n8:70_17": "pūjīyateti pūjito,",
  "anya-e0802n8:70_18": "pūjīyitthāti pūjito,",
  "anya-e0802n8:70_19": "apacāyitabboti apacāyito,",
  "anya-e0802n8:70_20": "mānitabbo pūjetabboti mānito,",
  "anya-e0802n8:70_21": "apacīyateti apacito,",
  "anya-e0802n8:70_22": "vandīyateti vandito,",
  "anya-e0802n8:70_23": "vandīyitthāti vandito,",
  "anya-e0802n8:70_24": "sakkarīyateti sakkārito,",
  "anya-e0802n8:70_25": "sakkarīyitthāti sakkārito.",
  "anya-e0802n8:71_0": "644, 661.Vepu sī dava vamu ku dā bhū hvādīhi thu ttima ṇimā nibbatte.",
  "anya-e0802n8:72_0": "Vepu sī dava vamu ku dā bhū huiccevamādīhi dhātūhi yathāsambhavaṁ thu ttima ṇimapaccayā honti nibbattatthe.",
  "anya-e0802n8:73_0": "Vepanaṁ vepo,",
  "anya-e0802n8:73_1": "tena nibbatto vepathu,",
  "anya-e0802n8:73_2": "sayanaṁ sayo,",
  "anya-e0802n8:73_3": "tena nibbatto sayathu,",
  "anya-e0802n8:73_4": "davanaṁ davo,",
  "anya-e0802n8:73_5": "tena nibbatto davathu.",
  "anya-e0802n8:73_6": "Vamanaṁ vamo,",
  "anya-e0802n8:73_7": "tena nibbatto vamathu.",
  "anya-e0802n8:73_8": "Kutti karaṇaṁ,",
  "anya-e0802n8:73_9": "tena nibbattaṁ kuttimaṁ.",
  "anya-e0802n8:73_10": "Dāti dānaṁ,",
  "anya-e0802n8:73_11": "tena nibbattaṁ dattimaṁ.",
  "anya-e0802n8:73_12": "Bhūti bhavanaṁ,",
  "anya-e0802n8:73_13": "tena nibbattaṁ bhottimaṁ.",
  "anya-e0802n8:73_14": "Avahuti avahanaṁ,",
  "anya-e0802n8:73_15": "tena nibbattaṁ ohāvimaṁ.",
  "anya-e0802n8:74_0": "645, 662.Akkosenamhāni.",
  "anya-e0802n8:75_0": "Akkosaiccetasmiṁ atthe namhi paṭisedhayutte ānipaccayo hoti dhātūhi.",
  "anya-e0802n8:76_0": "Na gamitabbaṁ agamāni te jamma desaṁ, na kattabbaṁ akarāṇi te jamma kammaṁ.",
  "anya-e0802n8:77_0": "Namhīti kimatthaṁ? Vipatti te jamma, vikati te jamma.",
  "anya-e0802n8:78_0": "Akkoseti kimatthaṁ? Na gantabbā agati te.",
  "anya-e0802n8:79_0": "646, 419.Ekādito sakissa kkhattuṁ.",
  "anya-e0802n8:80_0": "Ekādito sakissa kkhattuṁ hoti.",
  "anya-e0802n8:81_0": "Ekassa padatthassa sakiṁ vāraṁ ekakkhattuṁ,",
  "anya-e0802n8:81_1": "dvinnaṁ padatthānaṁ sakiṁ vāraṁ dvikkhattuṁ,",
  "anya-e0802n8:81_2": "tiṇṇaṁ padatthānaṁ sakiṁ vāraṁ tikkhattuṁ,",
  "anya-e0802n8:81_3": "evaṁ catukkhattuṁ,",
  "anya-e0802n8:81_4": "pañcakkhattuṁ,",
  "anya-e0802n8:81_5": "chakkhattuṁ,",
  "anya-e0802n8:81_6": "sattakkhattuṁ,",
  "anya-e0802n8:81_7": "aṭṭhakkhattuṁ,",
  "anya-e0802n8:81_8": "navakkhattuṁ,",
  "anya-e0802n8:81_9": "dasakkhattuṁ.",
  "anya-e0802n8:81_10": "Iccevamādayo saddā yojetabbā.",
  "anya-e0802n8:82_0": "647, 663.Sunassunassoṇa vānuvānūnunakhunānā.",
  "anya-e0802n8:83_0": "Sunaiccetassa pāṭipadikassa unassa oṇa vāna uvāna ūna unakha una ā ānādesā honti.",
  "anya-e0802n8:84_0": "Sāmikassa saddaṁ suṇātīti soṇo,",
  "anya-e0802n8:84_1": "sāmikassa saddaṁ suṇātīti svāno,",
  "anya-e0802n8:84_2": "evaṁ suvāno, sūno, sunakho, suno, sā, sāno.",
  "anya-e0802n8:85_0": "648, 664.Taruṇassa susu ca.",
  "anya-e0802n8:86_0": "Taruṇaiccetassa pāṭipadikassa susu ādeso hoti.",
  "anya-e0802n8:87_0": "Susu kāḷakeso.",
  "anya-e0802n8:88_0": "649, 665.Yuvassuvassuvuvānunūnā.",
  "anya-e0802n8:89_0": "Yuvaiccetassa pāṭipadikassa uvassa uvauvāna unaūnādesā honti.",
  "anya-e0802n8:90_0": "Yuvā, yuvāno, yuno, yūno.",
  "anya-e0802n8:91_0": "650, 651.Kāle vattamānātīte ṇvādayo.",
  "anya-e0802n8:92_0": "Kāle vattamānatthe ca atītatthe ca ṇu yu tapaccayā honti.",
  "anya-e0802n8:93_0": "Akāsi, karotīti kāru, agacchi, gacchatīti, vāyu, abhavi, bhavatīti bhūtaṁ.",
  "anya-e0802n8:94_0": "651, 647.Tavissatigamādīhi ṇī ghiṇa.",
  "anya-e0802n8:95_0": "Bhavissatikālatthe gamu bhaja su ṭhāiccevamādīhi dhātūhi ṇī ghiṇa paccayā honti.",
  "anya-e0802n8:96_0": "Āyatiṁ gamituṁ sīlaṁ yassa,",
  "anya-e0802n8:96_1": "so hotīti gāmī,",
  "anya-e0802n8:96_2": "āyatiṁ bhajituṁ sīlaṁ yassa,",
  "anya-e0802n8:96_3": "so hotīti bhājī,",
  "anya-e0802n8:96_4": "āyatiṁ passāpituṁ sīlaṁ yassa,",
  "anya-e0802n8:96_5": "so hotīti passāvi,",
  "anya-e0802n8:96_6": "āyatiṁ paṭṭhāyituṁ sīlaṁ yassa,",
  "anya-e0802n8:96_7": "so hotīti paṭṭhāyi.",
  "anya-e0802n8:97_0": "652, 648.Kriyāyaṁ ṇvu tavo.",
  "anya-e0802n8:98_0": "Kriyāyamatthe ṇvu tuiccete paccayā honti bhavissatikāle.",
  "anya-e0802n8:99_0": "‘‘Karissa’’nti kārako vajati, ‘‘bhuñjissa’’nti bhottā vajati.",
  "anya-e0802n8:100_0": "653, 306.Bhāvavācimhi catutthī.",
  "anya-e0802n8:101_0": "Bhāvavācimhi catutthīvibhatti hoti bhavissatikāle,",
  "anya-e0802n8:102_0": "Pacissate,",
  "anya-e0802n8:102_1": "pacanaṁ vā pāko,",
  "anya-e0802n8:102_2": "pākāya vajati.",
  "anya-e0802n8:102_3": "Bhuñjissate,",
  "anya-e0802n8:102_4": "bhojanaṁ vā bhogo,",
  "anya-e0802n8:102_5": "bhogāya vajati.",
  "anya-e0802n8:102_6": "Naccissate,",
  "anya-e0802n8:102_7": "naccanaṁ vā naccaṁ,",
  "anya-e0802n8:102_8": "naccāya vajati.",
  "anya-e0802n8:103_0": "654, 649.Kammaniṇo.",
  "anya-e0802n8:104_0": "Kammani upapade ṇapaccayo hoti bhavissatikāle.",
  "anya-e0802n8:105_0": "Nagaraṁ karissati nagarakāro vajati,",
  "anya-e0802n8:105_1": "sāliṁ lāvissati sālilāvo vajati,",
  "anya-e0802n8:105_2": "dhaññaṁ vapissati dhaññavāpo vajati,",
  "anya-e0802n8:105_3": "bhogaṁ dadissati bhogadāyo vajati,",
  "anya-e0802n8:105_4": "sindhuṁ pivissati sindhupāyo vajati.",
  "anya-e0802n8:106_0": "655, 650.Sese ssaṁ ntumānānā.",
  "anya-e0802n8:107_0": "Sesaiccetasmiṁ atthe ssaṁ ntu māna āna iccete paccayā honti bhavissatikāle kammūpapade.",
  "anya-e0802n8:108_0": "Kammaṁ karissati kammaṁ karissaṁ,",
  "anya-e0802n8:108_1": "evaṁ kammaṁ karonto,",
  "anya-e0802n8:108_2": "kammaṁ kurumāno,",
  "anya-e0802n8:108_3": "kammaṁ karāno vajati.",
  "anya-e0802n8:108_4": "Bhojanaṁ bhuñjissati bhojanaṁ bhuñjissaṁ,",
  "anya-e0802n8:108_5": "evaṁ bhojanaṁ bhuñjanto,",
  "anya-e0802n8:108_6": "bhojanaṁ bhuñjamāno,",
  "anya-e0802n8:108_7": "bhojanaṁ bhuñjāno vajati.",
  "anya-e0802n8:108_8": "Khādanaṁ khādissati khādanaṁ khādissaṁ,",
  "anya-e0802n8:108_9": "evaṁ khādanaṁ khādanto,",
  "anya-e0802n8:108_10": "khādanaṁ khādamāno,",
  "anya-e0802n8:108_11": "khādanaṁ khādāno vajati.",
  "anya-e0802n8:108_12": "Maggaṁ carissati maggaṁ carissaṁ,",
  "anya-e0802n8:108_13": "evaṁ maggaṁ caranto,",
  "anya-e0802n8:108_14": "maggaṁ caramāno,",
  "anya-e0802n8:108_15": "maggaṁ carāno vajati.",
  "anya-e0802n8:108_16": "Bhikkhaṁ bhikkhissati bhikkhaṁ bhikkhissaṁ,",
  "anya-e0802n8:108_17": "evaṁ bhikkhaṁ bhikkhanto,",
  "anya-e0802n8:108_18": "bhikkhaṁ bhikkhamāno,",
  "anya-e0802n8:108_19": "bhikkhaṁ bhikkhāno vajati.",
  "anya-e0802n8:109_0": "656, 666.Chavādītitatraṇa.",
  "anya-e0802n8:110_0": "Cha da ci ti su nī vi da pada tanu yata ada mada yuja vatumida mā pu ka la vara ve pu gupa dā iccevamādīhi",
  "anya-e0802n8:110_1": "dhātūhi tatraṇa iccete paccayā honti yathāsambhavaṁ.",
  "anya-e0802n8:111_0": "Ātapaṁ chādetīti chattaṁ, chatraṁ.",
  "anya-e0802n8:111_1": "Ārammaṇaṁ cintetīti cittaṁ, citraṁ.",
  "anya-e0802n8:111_2": "Cintenti sampayuttadhammā ethenāti vā cittaṁ, citraṁ.",
  "anya-e0802n8:111_3": "Atthe abhissavetīti suttaṁ, sutraṁ.",
  "anya-e0802n8:111_4": "Atthe sūcetīti vā suttaṁ, sutraṁ.",
  "anya-e0802n8:111_5": "Satte netīti nettaṁ, netraṁ.",
  "anya-e0802n8:111_6": "Satte icchitaṭṭhānaṁ nenti etenāti vā nettaṁ, netraṁ.",
  "anya-e0802n8:111_7": "Pakārena vidatīti pavittaṁ, pavitraṁ.",
  "anya-e0802n8:111_8": "Vividhena ākārena maṅga pāpaṁ punāti,",
  "anya-e0802n8:111_9": "sodhetīti pavittaṁ, pavitraṁ.",
  "anya-e0802n8:111_10": "Sucibhāvaṁ vā pāpuṇātīti pavittaṁ, pavitraṁ.",
  "anya-e0802n8:111_11": "Padati pāpuṇātīti patto, patro.",
  "anya-e0802n8:111_12": "Āhārā patanti ettha bhājaneti pattaṁ, patraṁ.",
  "anya-e0802n8:111_13": "Padati pavattatīti vā pattaṁ, patraṁ.",
  "anya-e0802n8:111_14": "Tanoti vitthāretīti tantaṁ, tantraṁ,",
  "anya-e0802n8:111_15": "tanitabbaṁ vitthāretabbanti vā tantaṁ, tantra.",
  "anya-e0802n8:111_16": "Yatatīti yattaṁ, yatraṁ.",
  "anya-e0802n8:111_17": "Yatati vīriyaṁ karoti etenāti vā yattaṁ, yatraṁ.",
  "anya-e0802n8:111_18": "Yatanaṁ vā yattaṁ, yatraṁ.",
  "anya-e0802n8:111_19": "Sukhadukkhaṁ adati bhakkhatīti attā, atrā.",
  "anya-e0802n8:111_20": "Madatīti mattaṁ, matraṁ.",
  "anya-e0802n8:111_21": "Vatthuṁ yujjanti etenāti yottaṁ, yotraṁ.",
  "anya-e0802n8:111_22": "Vattatīti vattaṁ, vatraṁ.",
  "anya-e0802n8:111_23": "Midati sinehaṁ karotīti mittaṁ. Mitraṁ.",
  "anya-e0802n8:111_24": "Midati sinehati etāyāti mettā, metrā.",
  "anya-e0802n8:111_25": "Pari samanta to sabbākārena minanti etāyāti mattā, matrā.",
  "anya-e0802n8:111_26": "Mānanaṁ vā mattaṁ, matraṁ.",
  "anya-e0802n8:111_27": "Attano kulaṁ punāti sodhetīti putto, putro.",
  "anya-e0802n8:111_28": "Kalitabbaṁ saṅkhyātabbanti kalattaṁ, kalatraṁ.",
  "anya-e0802n8:111_29": "Saṁsuṭṭhu vāreti etenāti varattaṁ, varatraṁ.",
  "anya-e0802n8:111_30": "Vepati kampatīti vettaṁ, vetraṁ.",
  "anya-e0802n8:111_31": "Gopitabbaṁ rakkhitabbanti guttaṁ. Gutraṁ, gottaṁ, gotraṁ.",
  "anya-e0802n8:111_32": "Dāti avakhaṇḍati etenāti dāttaṁ,",
  "anya-e0802n8:111_33": "dātraṁ iccevamādi.",
  "anya-e0802n8:112_0": "657, 667.Vadādīhi ṇitto gaṇe.",
  "anya-e0802n8:113_0": "Vada cara varaiccevamādīhi dhātūhi ṇittapaccayo hoti gaṇatthe.",
  "anya-e0802n8:114_0": "Vāditānaṁ gaṇo vādittaṁ. Evaṁ cārittaṁ, vārittaṁ, iccevamādi.",
  "anya-e0802n8:115_0": "658, 668.Midādīhi ttitiyo.",
  "anya-e0802n8:116_0": "Mida pada ranja tanu dhāiccevamādīhi dhātūhi tti ti iccete paccayā honti.",
  "anya-e0802n8:117_0": "Midati sinehatīti metti,",
  "anya-e0802n8:117_1": "padati gacchatīti patti,",
  "anya-e0802n8:117_2": "ranjati etthāti ratti,",
  "anya-e0802n8:117_3": "tanoti vitthāretīti tanti,",
  "anya-e0802n8:117_4": "attano kulaṁ tanoti vitthāretīti vā tanti,",
  "anya-e0802n8:117_5": "paresaṁ itthīnaṁ puttaṁ dhāretīti dhāti,",
  "anya-e0802n8:117_6": "khīraṁ dhāretīti vā dhāti,",
  "anya-e0802n8:117_7": "attano sabhāvaṁ dhāretīti vā dhāti iccevamādi.",
  "anya-e0802n8:118_0": "659, 669.Usu ranja daṁsānaṁ daṁsassa daḍḍhoḍha ṭhā ca.",
  "anya-e0802n8:119_0": "Usu ranja daṁsaiccetesaṁ dhātūnaṁ daṁsassa daḍḍhādeso hoti, ḍha ṭhapaccayā ca honti.",
  "anya-e0802n8:120_0": "Usīyate uḍḍho, ranjanti etthāti raṭṭhaṁ, daṁsīyateti daḍḍho.",
  "anya-e0802n8:121_0": "660, 670.Sūvusānamūvusānamatothoca.",
  "anya-e0802n8:122_0": "Sūvu asaiccetesaṁ dhātūnaṁ ūuasānaṁ atādeso hoti, thapaccayo ca.",
  "anya-e0802n8:123_0": "Savati hiṁsati etenāti satthaṁ,",
  "anya-e0802n8:123_1": "hirottappaṁ saṁvarati etenātivatthaṁ,",
  "anya-e0802n8:123_2": "saddānurūpaṁ asatibhavatīti attho,",
  "anya-e0802n8:124_0": "661, 671.Ranjudādīhidha didda kirā kvaci jadalopoca.",
  "anya-e0802n8:125_0": "Ranja uda idi cadi madi khuda chidi rudi dala susa suca vaca vaja iccevamādīhi dhātūhi dha da idda ka iraiccete paccayā honti,",
  "anya-e0802n8:125_1": "kvaci ja da lopo ca,",
  "anya-e0802n8:125_2": "puna nippajjante.",
  "anya-e0802n8:126_0": "Rañjitabbanti randhaṁ,",
  "anya-e0802n8:126_1": "ranjayitthāti vā randhaṁ,",
  "anya-e0802n8:126_2": "attani sannissitānaṁ macchamakarānaṁ pītisomanassaṁ undati pasavati janetīti samuddo,",
  "anya-e0802n8:126_3": "indati paramissariyaṁ karotīti indo,",
  "anya-e0802n8:126_4": "indattaṁ adhipatibhāvaṁ karotīti vā indo,",
  "anya-e0802n8:126_5": "canditabbo icchitabboti cando,",
  "anya-e0802n8:126_6": "mandati hāsetīti mando,",
  "anya-e0802n8:126_7": "maditabbo hāsetabboti vā mando,",
  "anya-e0802n8:126_8": "khudati pipāsetīti khuddo,",
  "anya-e0802n8:126_9": "chinditabboti chiddo,",
  "anya-e0802n8:126_10": "rudati hiṁsatīti ruddo,",
  "anya-e0802n8:126_11": "dalati duggatabhāvaṁ gacchatīti daliddo,",
  "anya-e0802n8:126_12": "sussatīti sukkaṁ,",
  "anya-e0802n8:126_13": "sucatītisoko,",
  "anya-e0802n8:126_14": "vacitabbanti vakkaṁ,",
  "anya-e0802n8:126_15": "appaṭihato hutvā vajati gacchatīti vajiraṁ iccevamādi.",
  "anya-e0802n8:127_0": "662, 672.Paṭito hissa heraṇa hīraṇa.",
  "anya-e0802n8:128_0": "Paṭiiccetasmā hissa dhātussa heraṇa hīraṇa ādesā honti.",
  "anya-e0802n8:129_0": "Paṭipakkhemadditvāgacchati pavattatīti pāṭiheraṁ, pāṭihīraṁ.",
  "anya-e0802n8:130_0": "663, 673.Kaḍyādīhiko.",
  "anya-e0802n8:131_0": "Kaḍi ghaḍi vaḍi karaḍi maḍi saḍi kuṭhi bhaḍi paḍi daḍi raḍi taḍi isiḍi caḍi gaḍi aḍi laḍi meḍi eraḍi",
  "anya-e0802n8:131_1": "khaḍi iccevamādīhi dhātūhi kapaccayo hoti saha paccayena ca nippajjante yathāsambhavaṁ.",
  "anya-e0802n8:132_0": "Kaṇḍitabbo chinditabboti kaṇḍo,",
  "anya-e0802n8:132_1": "ghaṇḍitabbo ghaṭetabboti ghaṇḍo,",
  "anya-e0802n8:132_2": "vaṇḍanti etthāti vaṇḍo,",
  "anya-e0802n8:132_3": "karaṇḍitabbo bhājetabboti karaṇḍo,",
  "anya-e0802n8:132_4": "maṇḍīyate vibhūsīyate etenāti maṇḍo,",
  "anya-e0802n8:132_5": "saṇḍanti gumbanti etthāti saṇḍo,",
  "anya-e0802n8:132_6": "aṅgamaṅgāni kuṇṭhati chindatīti kuṭṭhaṁ,",
  "anya-e0802n8:132_7": "bhaṇḍitabbanti bhaṇḍaṁ,",
  "anya-e0802n8:132_8": "paṇḍati liṅgavekallabhāvaṁ gacchatīti paṇḍako.",
  "anya-e0802n8:132_9": "Daṇḍati āṇaṁ karoti etenāti daṇḍo,",
  "anya-e0802n8:132_10": "raṇḍati hiṁsatīti raṇḍo,",
  "anya-e0802n8:132_11": "visesena taṇḍati cāleti paresaṁ viññūnaṁ hadayaṁ kampetīti vitaṇḍo,",
  "anya-e0802n8:132_12": "isiṇḍati paresaṁ maddatīti isiṇḍo,",
  "anya-e0802n8:132_13": "caṇḍati caṇḍikkabhāvaṁ karotīti caṇḍo,",
  "anya-e0802n8:132_14": "gaṇḍati sannicayati samūhaṁ karoti etthāti gaṇḍo,",
  "anya-e0802n8:132_15": "aṇḍīyati nibbattīyatīti aṇḍo,",
  "anya-e0802n8:132_16": "laṇḍitabbo jigucchitabboti laṇḍo,",
  "anya-e0802n8:132_17": "meṇḍati kuṭilabhāvaṁ gacchatīti meṇḍo,",
  "anya-e0802n8:132_18": "eraṇḍati rogaṁ hiṁsatīti eraṇḍo,",
  "anya-e0802n8:132_19": "khaṇḍitabbo chinditabboti khaṇḍo iccevamādi.",
  "anya-e0802n8:133_0": "664, 674.Khādāmagamānaṁ kha ndha’ndha gandhā.",
  "anya-e0802n8:134_0": "Khāda ama gamuiccetesaṁ dhātūnaṁ khandha andha gandhādesā honti, kapaccayo ca hoti.",
  "anya-e0802n8:135_0": "Jātijarāmaraṇādīhi saṁsāradukkhehi khāditabboti khandho,",
  "anya-e0802n8:135_1": "amati aṅgamaṅgassa rujjanabhāvaṁ gacchatīti andho,",
  "anya-e0802n8:135_2": "cakkhunā amati rujjatīti vā andho,",
  "anya-e0802n8:135_3": "taṁ taṁ ṭhānaṁ vātena gacchatīti gandho.",
  "anya-e0802n8:135_4": "Evaṁ khandhako, andhako, gandhako.",
  "anya-e0802n8:136_0": "665, 675.Paṭādīhyalaṁ.",
  "anya-e0802n8:137_0": "Paṭa kala kusa kada bhaganda mekha vakka takka palla sadda mūla bila vida caḍaṁ pañca vā vasa paci maca",
  "anya-e0802n8:137_1": "musa gotthu puthu bahu maṅga baha kamba samba aggaiccevamādīhi dhātūhi pāṭi padikehi ca uttarapadesu",
  "anya-e0802n8:137_2": "alapaccayo hoti,",
  "anya-e0802n8:137_3": "pacchā puna nippajjante.",
  "anya-e0802n8:138_0": "Paṭe alanti paṭalaṁ,",
  "anya-e0802n8:138_1": "kale alanti kalalaṁ,",
  "anya-e0802n8:138_2": "pāpake akusale dhamme kusati chindatīti kusalaṁ,",
  "anya-e0802n8:138_3": "kusabhūte yathāsabhāvadhamme alanti vā kusalaṁ,",
  "anya-e0802n8:138_4": "kuse uddissa dāne alanti vā kusalaṁ,",
  "anya-e0802n8:138_5": "kuse sañcaye dhammasamudāye alanti vā kusalaṁ,",
  "anya-e0802n8:138_6": "kadde madde alanti kadalaṁ,",
  "anya-e0802n8:138_7": "bhagande secane alanti bhagandalaṁ,",
  "anya-e0802n8:138_8": "bhagande muttakarīsaharaṇe alanti vā bhagandalaṁ,",
  "anya-e0802n8:138_9": "mekhe kaṭivicitte alanti mekhalaṁ,",
  "anya-e0802n8:138_10": "vakke rukkhatace alanti vakkalaṁ,",
  "anya-e0802n8:138_11": "takke rukkhasilese alanti takkalaṁ,",
  "anya-e0802n8:138_12": "palle ninnaṭṭhāne alanti pallalaṁ,",
  "anya-e0802n8:138_13": "sadde harite alanti saddalaṁ,",
  "anya-e0802n8:138_14": "mūle patiṭṭhāne alanti mulālaṁ,",
  "anya-e0802n8:138_15": "biḷe nissaye alanti bilālaṁ,",
  "anya-e0802n8:138_16": "vide vijjamāne alanti vidalaṁ,",
  "anya-e0802n8:138_17": "caṇḍe alanti caṇḍālo,",
  "anya-e0802n8:138_18": "pañcannaṁ rājūnaṁ alanti pañcālo,",
  "anya-e0802n8:138_19": "vā gatigandhanesu alanti vālaṁ,",
  "anya-e0802n8:138_20": "vā padagamane alanti vā vāḷo,",
  "anya-e0802n8:138_21": "vase acchādane alanti vasalo,",
  "anya-e0802n8:138_22": "pace vitthāre alanti pacalo,",
  "anya-e0802n8:138_23": "mace corakamme alanti macalo,",
  "anya-e0802n8:138_24": "muse theyye,",
  "anya-e0802n8:138_25": "muse pāṇacāge vā alanti musalo,",
  "anya-e0802n8:138_26": "gotte vaṁse siṅgālajātiyaṁ alanti gotthulo,",
  "anya-e0802n8:138_27": "puthumhi vitthāre alanti puthulo,",
  "anya-e0802n8:138_28": "bahumhi saṅkhyāne alanti bahulo,",
  "anya-e0802n8:138_29": "bahumhi vuddhimhi alanti vā bahulo,",
  "anya-e0802n8:138_30": "maṅgamhi gamane alanti maṅgalaṁ,",
  "anya-e0802n8:138_31": "bahumhi vuddhimhi alanti bahalaṁ,",
  "anya-e0802n8:138_32": "kambamhi sañcalane alanti kambalaṁ.",
  "anya-e0802n8:138_33": "Sambamhi maṇḍale alanti sambalaṁ,",
  "anya-e0802n8:138_34": "agge gatikoṭille alanti aggaḷaṁ.",
  "anya-e0802n8:138_35": "Iccevamādayo aññepi saddā bhavanti.",
  "anya-e0802n8:139_0": "666, 676.Puthassa puthu pathā mo vā.",
  "anya-e0802n8:140_0": "Puthaiccetassa pāṭipadikassa puthu pathādesā honti, kvaci amapaccayo hoti.",
  "anya-e0802n8:141_0": "Putha hutvā jātanti puthavī pathame jāto pathamo, pathavī,",
  "anya-e0802n8:141_1": "paṭhamo vā,",
  "anya-e0802n8:141_2": "puthu kilese janetīti puthujjano,",
  "anya-e0802n8:141_3": "puthu hutvā jātanti pathavī,",
  "anya-e0802n8:141_4": "pathavī vā.",
  "anya-e0802n8:142_0": "667, 677.Sasvādīhi tu davo.",
  "anya-e0802n8:143_0": "Sasu dada ada madaiccevamādīhi dhātūhi tu duiccete paccayā honti.",
  "anya-e0802n8:144_0": "Aññe satte sasati hiṁsatīti sattu,",
  "anya-e0802n8:144_1": "dukkhaṁ dadātīti daddu,",
  "anya-e0802n8:144_2": "dukkhena adati bhakkhati etthāti addu,",
  "anya-e0802n8:144_3": "dukkhaṁ adati anubhavati jano etenāti vā addu,",
  "anya-e0802n8:144_4": "dukkhaṁ bhājanaṁ ādhāraṁ bhāvatīti vā addu,",
  "anya-e0802n8:144_5": "madati ummattaṁ karotīti maddu,",
  "anya-e0802n8:144_6": "madati maddabhāvaṁ karotīti vā maddu.",
  "anya-e0802n8:145_0": "668, 678.Jhādīhi īvaro.",
  "anya-e0802n8:146_0": "Ci pā dhāiccevamādīhi dhātūhi īvarapaccayohoti.",
  "anya-e0802n8:147_0": "Cīyatīti cīvaraṁ,",
  "anya-e0802n8:147_1": "pivatīti pīvaro,",
  "anya-e0802n8:147_2": "pātabbaṁ rakkhitabbanti vā pīvaraṁ.",
  "anya-e0802n8:147_3": "Dhāreti dhāretvā jīvitaṁ kappetīti dhīvaro, dhīvaraṁ.",
  "anya-e0802n8:148_0": "669, 679.Munādīhi ci.",
  "anya-e0802n8:149_0": "Muna yata agga pata kava suca ruca mahāla bhaddāla manaiiccevamādīhi dhātūhi,",
  "anya-e0802n8:149_1": "pāṭipadikehi ca ipaccayo hoti.",
  "anya-e0802n8:150_0": "Atthānatthaṁ munāti,",
  "anya-e0802n8:150_1": "ñeyyadhammaṁ lakkhaṇādivasena vā jānātīti muni,",
  "anya-e0802n8:150_2": "yatati vīriyaṁ karotīti yati,",
  "anya-e0802n8:150_3": "aggati kuṭilabhāvaṁ gacchatīti aggi,",
  "anya-e0802n8:150_4": "patati seṭṭho hutvā purato gacchatīti pati,",
  "anya-e0802n8:150_5": "kabyaṁ bandhatīti kavi,",
  "anya-e0802n8:150_6": "kantaṁ manāpavacanaṁ vadatīti vā kavi.",
  "anya-e0802n8:150_7": "Sucati parisuddhaṁ bhavatīti suci,",
  "anya-e0802n8:150_8": "rucati dibbatīti ruci,",
  "anya-e0802n8:150_9": "mahantaṁ vibhāvaṁ bhogakkhandhaṁ lātīti mahāli,",
  "anya-e0802n8:150_10": "bhaddaṁ yasaṁ lātīti bhaddāli,",
  "anya-e0802n8:150_11": "manaṁ tattha ratane nayatīti maṇi.",
  "anya-e0802n8:151_0": "670, 680.Vidādīhyūro.",
  "anya-e0802n8:152_0": "Vida valla masa sida dukukapu maya udi khajja kuraiccevamādīhi dhātūhi,",
  "anya-e0802n8:152_1": "pāṭipadikehi ca ūrapaccayohoti.",
  "anya-e0802n8:153_0": "Vidituṁ alanti vidūro,",
  "anya-e0802n8:153_1": "vidūraṭṭhāne jāto vedūro,",
  "anya-e0802n8:153_2": "vallati vallabhāvena bhavatīti vallūro,",
  "anya-e0802n8:153_3": "vallati aññamaññaṁ bandhatīti vā vallūro,",
  "anya-e0802n8:153_4": "āmasitabboti masūro,",
  "anya-e0802n8:153_5": "sindati siṅgārabhāvaṁ gacchatīti sindūro,",
  "anya-e0802n8:153_6": "sindati virocatīti vā sindūro,",
  "anya-e0802n8:153_7": "gamituṁ alaṁ anāsannattāti dūro,",
  "anya-e0802n8:153_8": "kuti saddaṁ karotīti kūro,",
  "anya-e0802n8:153_9": "attano gandhena aññaṁ gandhaṁ kapati hanati hiṁsatīti kappūro,",
  "anya-e0802n8:153_10": "kappati rogāpanayane samatthetīti vā kappūro,",
  "anya-e0802n8:153_11": "mahiyaṁ ravatīti mayūro,",
  "anya-e0802n8:153_12": "mahiyaṁ yāti gacchatīti mayūro,",
  "anya-e0802n8:153_13": "paṁsuṁ undati pasavatīti undūro,",
  "anya-e0802n8:153_14": "khajjitabbo khāditabboti khajjūro,",
  "anya-e0802n8:153_15": "kurati akkosatīti kurūro.",
  "anya-e0802n8:154_0": "671, 681.Hanādīhi ṇu nutavo.",
  "anya-e0802n8:155_0": "Hana jana bhāri khanu ama ve vedhā siki hi iccevamādīhi dhātūhi ṇu nu kuiccete paccayā honti.",
  "anya-e0802n8:156_0": "Bhojanaṁ hanati hiṁsati etenāti haṇu, hanu vā.",
  "anya-e0802n8:156_1": "Gamanaṁ janetītī jāṇu,",
  "anya-e0802n8:156_2": "bhāṇu dibbatīti bhāṇu,",
  "anya-e0802n8:156_3": "nivāte riti gacchatītī reṇu,",
  "anya-e0802n8:156_4": "khaṇitabbo avadāritabboti khāṇu.",
  "anya-e0802n8:156_5": "Aṅgamaṅgassa rujjanabhāvaṁ vijjhanabhāvaṁ amati gacchatīti aṇu, veṇu,",
  "anya-e0802n8:156_6": "veti tantasantāne bhavatītī veṇu,",
  "anya-e0802n8:156_7": "bahisārealantivāveṇu vacchaṁ,",
  "anya-e0802n8:156_8": "pāyetītī dhenu,",
  "anya-e0802n8:156_9": "atthaṁ dhāretītī dhātu,",
  "anya-e0802n8:156_10": "gamanapacanādikaṁ kriyaṁ dhāretītī vā dhātu,",
  "anya-e0802n8:156_11": "sīyatī bandhīyatītī setu,",
  "anya-e0802n8:156_12": "uddhaṁ gacchati pavattatītī ketu,",
  "anya-e0802n8:156_13": "attano phalaṁ hinoti pavattatītī hetu.",
  "anya-e0802n8:157_0": "672, 682.Kuṭādīhi ṭho.",
  "anya-e0802n8:158_0": "Kuṭa kusa kaṭaiccevamādīhi dhātūhi, pāṭipadikehi ca ṭhapaccayo hotī.",
  "anya-e0802n8:159_0": "Aṅgamaṅgaṁ kuṭati chindatītī kuṭṭhaṁ,",
  "anya-e0802n8:159_1": "dhaññena chādetabbo pūretabbotī koṭṭho,",
  "anya-e0802n8:159_2": "kaṭitabbaṁ madditabbanti, kaṭṭhaṁ.",
  "anya-e0802n8:160_0": "673, 683.Manu pūra suṇādīhi ussa nusisā.",
  "anya-e0802n8:161_0": "Manu pūra suṇa kusu ila ala maha si ki iccevamādīhi dhātūhi,",
  "anya-e0802n8:161_1": "pāṭipadikehi ca usa nusa isaiccete paccayā honti,",
  "anya-e0802n8:161_2": "puna nippajjante.",
  "anya-e0802n8:162_0": "Kusalākusale dhamme manati jānātīti manusso, mānuso.",
  "anya-e0802n8:162_1": "Kāraṇā kāraṇaṁ manati jānātīti vā manusso, mānusso.",
  "anya-e0802n8:162_2": "Atthānatthaṁ manati jānātīti vā manusso, mānusso.",
  "anya-e0802n8:162_3": "Mātāpitūnaṁ hadayaṁ pūretīti puriso,",
  "anya-e0802n8:162_4": "attano manorathaṁ pūretīti vā puriso,",
  "anya-e0802n8:162_5": "pūretīti vā poso,",
  "anya-e0802n8:162_6": "sasurehi suṇitabbā hiṁsitabbāti suṇisā,",
  "anya-e0802n8:162_7": "dvinnaṁ jānānaṁ kulasantānaṁ karotīti vā suṇisā,",
  "anya-e0802n8:162_8": "kucchitabbanti karīsaṁ,",
  "anya-e0802n8:162_9": "gabbhaṁ vimocetīti suriso,",
  "anya-e0802n8:162_10": "tamandhakāravidhamanena sattānaṁ bhayaṁ surati hiṁsatīti sūriyo,",
  "anya-e0802n8:162_11": "rogaṁ hiṁsatīti sirīso,",
  "anya-e0802n8:162_12": "ilati kampatīti illiso,",
  "anya-e0802n8:162_13": "taṇhāya dubbalo hutvā ilati kampatīti vā illigho,",
  "anya-e0802n8:162_14": "pāpakaraṇe alati samatthetīti alaso,",
  "anya-e0802n8:162_15": "mahitabbo pūjetabboti mahiso,",
  "anya-e0802n8:162_16": "sīyati bandhīyatīti sīsaṁ,",
  "anya-e0802n8:162_17": "kitabbaṁ hiṁsitabbanti kisaṁ,",
  "anya-e0802n8:162_18": "iccevamādi.",
  "anya-e0802n8:163_0": "Iti kibbidhānakappe uṇādikappo chaṭṭho kaṇḍo.",
  "anya-e0802n8:164_0": "Uṇādikappo niṭṭhito.",
  "anya-e0802n8:165_0": "Kaccāyanapakaraṇaṁ niṭṭhitaṁ."
}