{
  "tha-ap90:0.1_0": "Therāpadāna",
  "tha-ap90:0.2_0": "Timiravagga",
  "tha-ap90:0.3_0": "8. Saddasaññakattheraapadāna",
  "tha-ap90:1.1_0": "“Pabbate himavantamhi,",
  "tha-ap90:1.2_0": "vasāmi paṇṇasanthare;",
  "tha-ap90:1.3_0": "Phussassa dhammaṁ bhaṇato,",
  "tha-ap90:1.4_0": "sadde cittaṁ pasādayiṁ.",
  "tha-ap90:2.1_0": "Dvenavute ito kappe,",
  "tha-ap90:2.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap90:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap90:2.4_0": "puññakammassidaṁ phalaṁ.",
  "tha-ap90:3.1_0": "Paṭisambhidā catasso,",
  "tha-ap90:3.2_0": "…pe…",
  "tha-ap90:3.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap90:4.1_0": "Itthaṁ sudaṁ āyasmā saddasaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap90:5.1_0": "Saddasaññakattherassāpadānaṁ aṭṭhamaṁ."
}