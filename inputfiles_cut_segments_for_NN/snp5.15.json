{
  "snp5.15:0.1_0": "Sutta Nipāta 5.15",
  "snp5.15:0.2_0": "Posālamāṇavapucchā",
  "snp5.15:1.1_0": "“Yo atītaṁ ādisati,",
  "snp5.15:1.2_0": "(iccāyasmā posālo)",
  "snp5.15:1.3_0": "Anejo chinnasaṁsayo;",
  "snp5.15:1.4_0": "Pāraguṁ sabbadhammānaṁ,",
  "snp5.15:1.5_0": "Atthi pañhena āgamaṁ.",
  "snp5.15:2.1_0": "Vibhūtarūpasaññissa,",
  "snp5.15:2.2_0": "sabbakāyappahāyino;",
  "snp5.15:2.3_0": "Ajjhattañca bahiddhā ca,",
  "snp5.15:2.4_0": "natthi kiñcīti passato;",
  "snp5.15:2.5_0": "Ñāṇaṁ sakkānupucchāmi,",
  "snp5.15:2.6_0": "kathaṁ neyyo tathāvidho”.",
  "snp5.15:3.1_0": "“Viññāṇaṭṭhitiyo sabbā,",
  "snp5.15:3.2_0": "(posālāti bhagavā)",
  "snp5.15:3.3_0": "Abhijānaṁ tathāgato;",
  "snp5.15:3.4_0": "Tiṭṭhantamenaṁ jānāti,",
  "snp5.15:3.5_0": "Vimuttaṁ tapparāyaṇaṁ.",
  "snp5.15:4.1_0": "Ākiñcaññasambhavaṁ ñatvā,",
  "snp5.15:4.2_0": "Nandī saṁyojanaṁ iti;",
  "snp5.15:4.3_0": "Evametaṁ abhiññāya,",
  "snp5.15:4.4_0": "Tato tattha vipassati;",
  "snp5.15:4.5_0": "Etaṁ ñāṇaṁ tathaṁ tassa,",
  "snp5.15:4.6_0": "Brāhmaṇassa vusīmato”ti.",
  "snp5.15:4.7_0": "Posālamāṇavapucchā cuddasamā."
}