{
  "anya-e0601n0:0_0": " Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e0601n0:1_0": "Namakkārapāḷi",
  "anya-e0601n0:2_0": "1.",
  "anya-e0601n0:3_0": " Sugataṁ sugataṁ seṭṭhaṁ, kusalaṁkusalaṁ jahaṁ;",
  "anya-e0601n0:4_0": "Amataṁ amataṁ santaṁ, asamaṁ asamaṁ dadaṁ.",
  "anya-e0601n0:5_0": "Saraṇaṁ saraṇaṁ lokaṁ, araṇaṁ araṇaṁ karaṁ;",
  "anya-e0601n0:6_0": "Abhayaṁ abhayaṁ ṭhānaṁ, nāyakaṁ nāyakaṁ name.",
  "anya-e0601n0:7_0": "2.",
  "anya-e0601n0:8_0": " Nayanasubhagakāyaṅgaṁ,",
  "anya-e0601n0:9_0": "Madhuravarasaropetaṁ;",
  "anya-e0601n0:10_0": "Amitaguṇagaṇādhāraṁ,",
  "anya-e0601n0:11_0": "Dasabalamatulaṁ vande.",
  "anya-e0601n0:12_0": "3.",
  "anya-e0601n0:13_0": " Yo buddho dhitimāññadhārako,",
  "anya-e0601n0:14_0": "Saṁsāre anubhosi kāyikaṁ;",
  "anya-e0601n0:15_0": "Dukkhaṁ cetasikañca lokato,",
  "anya-e0601n0:16_0": "Taṁ vande naradevamaṅgalaṁ.",
  "anya-e0601n0:17_0": "4.",
  "anya-e0601n0:18_0": " Bāttiṁsatilakkhaṇacitradehaṁ,",
  "anya-e0601n0:19_0": "Dehajjutiniggatapajjalantaṁ;",
  "anya-e0601n0:20_0": "Paññādhitisīlaguṇoghavindaṁ,",
  "anya-e0601n0:21_0": "Vande munimantimajātiyuttaṁ.",
  "anya-e0601n0:22_0": "5.",
  "anya-e0601n0:23_0": " Pātodayaṁ bāladivākaraṁva,",
  "anya-e0601n0:24_0": "Majjhe yatīnaṁ lalitaṁ sirīhi;",
  "anya-e0601n0:25_0": "Puṇṇindusaṅkāsamukhaṁ anejaṁ,",
  "anya-e0601n0:26_0": "Vandāmi sabbaññumahaṁ munindaṁ.",
  "anya-e0601n0:27_0": "6.",
  "anya-e0601n0:28_0": " Upetapuñño varabodhimūle,",
  "anya-e0601n0:29_0": "Sasenamāraṁ sugato jinitvā;",
  "anya-e0601n0:30_0": "Abojjhi bodhiṁ aruṇodayamhi,",
  "anya-e0601n0:31_0": "Namāmi taṁ mārajinaṁ abhaṅgaṁ.",
  "anya-e0601n0:32_0": "7.",
  "anya-e0601n0:33_0": " Rāgādichedāmalañāṇakhaggaṁ,",
  "anya-e0601n0:34_0": "Satīsamaññāphalakābhigāhaṁ;",
  "anya-e0601n0:35_0": "Sīloghalaṅkāravibhūsitaṁ taṁ,",
  "anya-e0601n0:36_0": "Namāmibhiññāvaramiddhupetaṁ.",
  "anya-e0601n0:37_0": "8.",
  "anya-e0601n0:38_0": " Dayālayaṁ sabbadhi dukkaraṁ karaṁ,",
  "anya-e0601n0:39_0": "Bhavaṇṇavātikkamamaggataṁ gataṁ;",
  "anya-e0601n0:40_0": "Tilokanāthaṁ susamāhitaṁ hitaṁ,",
  "anya-e0601n0:41_0": "Samantacakkhuṁ paṇamāmi taṁmitaṁ.",
  "anya-e0601n0:42_0": "9.",
  "anya-e0601n0:43_0": " Tahiṁ tahiṁ pāramisañcayaṁ cayaṁ,",
  "anya-e0601n0:44_0": "Gataṁ gataṁ sabbhi sukhappadaṁ padaṁ;",
  "anya-e0601n0:45_0": "Narānarānaṁ sukhasambhavaṁ bhavaṁ,",
  "anya-e0601n0:46_0": "Namānamānaṁ jinapuṅgavaṁ gavaṁ.",
  "anya-e0601n0:47_0": "10.",
  "anya-e0601n0:48_0": " Maggaṅganāvaṁ munidakkhanāviko,",
  "anya-e0601n0:49_0": "Īhāphiyaṁ ñāṇakarena gāhako;",
  "anya-e0601n0:50_0": "Āruyha yo tāya bahū bhavaṇṇavā,",
  "anya-e0601n0:51_0": "Tāresi taṁ buddhamaghappahaṁ name.",
  "anya-e0601n0:52_0": "11.",
  "anya-e0601n0:53_0": " Samatiṁsatipāramisambharaṇaṁ,",
  "anya-e0601n0:54_0": "Varabodhidume catusaccadasaṁ;",
  "anya-e0601n0:55_0": "Varamiddhigataṁ naradevahitaṁ,",
  "anya-e0601n0:56_0": "Tibhavūpasamaṁ paṇamāmi jinaṁ.",
  "anya-e0601n0:57_0": "12.",
  "anya-e0601n0:58_0": " Satapuññajalakkhaṇikaṁ virajaṁ,",
  "anya-e0601n0:59_0": "Gaganūpamadhiṁ dhitimerusamaṁ;",
  "anya-e0601n0:60_0": "Jalajūpamasītalasīlayutaṁ,",
  "anya-e0601n0:61_0": "Pathavīsahanaṁ paṇamāmi jinaṁ.",
  "anya-e0601n0:62_0": "13.",
  "anya-e0601n0:63_0": " Yo buddho sumati dive divākarova,",
  "anya-e0601n0:64_0": "Sobhanto ratijanane silāsanamhi;",
  "anya-e0601n0:65_0": "Āsīno sivasukhadaṁ adesi dhammaṁ,",
  "anya-e0601n0:66_0": "Devānaṁ tamasadisaṁ namāmi niccaṁ.",
  "anya-e0601n0:67_0": "14.",
  "anya-e0601n0:68_0": " Yo pādapaṅkajamuduttalarājikehi,",
  "anya-e0601n0:69_0": "Lokehi tīhivikalehi nirākulehi;",
  "anya-e0601n0:70_0": "Sampāpuṇe nirupameyyatameva nātho,",
  "anya-e0601n0:71_0": "Taṁ sabbalokamahitaṁ asamaṁ namāmi.",
  "anya-e0601n0:72_0": "15.",
  "anya-e0601n0:73_0": " Buddhaṁ narānarasamosaraṇaṁ dhitattaṁ,",
  "anya-e0601n0:74_0": "Paññāpadīpajutiyā vihatandhakāraṁ;",
  "anya-e0601n0:75_0": "Atthābhikāmanaradevahitāvahaṁ taṁ,",
  "anya-e0601n0:76_0": "Vandāmi kāruṇikamaggamanantañāṇaṁ.",
  "anya-e0601n0:77_0": "16.",
  "anya-e0601n0:78_0": " Akhilaguṇanidhāno yo munindopagantvā,",
  "anya-e0601n0:79_0": "Vanamisipatanavhaṁ saññatānaṁ niketaṁ;",
  "anya-e0601n0:80_0": "Tahimakusalachedaṁ dhammacakkaṁ pavatto,",
  "anya-e0601n0:81_0": "Tamatulamabhikantaṁ vandaneyyaṁ namāmi.",
  "anya-e0601n0:82_0": "17.",
  "anya-e0601n0:83_0": " Suciparivāritaṁ surucirappabhāhi rattaṁ,",
  "anya-e0601n0:84_0": "Sirivisarālayaṁ gupitamindriyehupetaṁ;",
  "anya-e0601n0:85_0": "Ravisasimaṇḍalappabhutilakkhaṇopacittaṁ,",
  "anya-e0601n0:86_0": "Suranarapūjitaṁ sugatamādaraṁ namāmi.",
  "anya-e0601n0:87_0": "18.",
  "anya-e0601n0:88_0": " Maggoḷumpena muhapaṭighāsādiullolavīciṁ,",
  "anya-e0601n0:89_0": "Saṁsāroghaṁ tari tamabhayaṁ pārapattaṁ pajānaṁ;",
  "anya-e0601n0:90_0": "Tāṇaṁ leṇaṁ asamasaraṇaṁ ekatitthaṁ patiṭṭhaṁ,",
  "anya-e0601n0:91_0": "Puññakkhettaṁ paramasukhadaṁ dhammarājaṁ namāmi.",
  "anya-e0601n0:92_0": "19.",
  "anya-e0601n0:93_0": " Kaṇḍambaṁmūle parahitakaro yo munindo nisinno,",
  "anya-e0601n0:94_0": "Accheraṁ sīghaṁ nayanasubhagaṁ ākulaṇṇaggijālaṁ;",
  "anya-e0601n0:95_0": "Dujjāladdhaṁsaṁ munibhijahitaṁ pāṭiheraṁ akāsi,",
  "anya-e0601n0:96_0": "Vande taṁ seṭṭhaṁ paramaratijaṁ iddhidhammehupetaṁ.",
  "anya-e0601n0:97_0": "20.",
  "anya-e0601n0:98_0": " Munindakko yveko dayudayaruṇo ñāṇavitthiṇṇabimbo,",
  "anya-e0601n0:99_0": "Vineyyappāṇoghaṁ kamalakathitaṁ dhammaraṁsīvarehi;",
  "anya-e0601n0:100_0": "Subodhesī suddhe tibhavakuhare byāpitakkittinañca,",
  "anya-e0601n0:101_0": "Tilokekaccakkhuṁ dukhamasahanaṁ taṁ mahesiṁ namāmi.",
  "anya-e0601n0:102_0": "21.",
  "anya-e0601n0:103_0": " Yo jino anekajātiyaṁ saputtadāramaṅgajīvitampi,",
  "anya-e0601n0:104_0": "Bodhipemato alaggamānaso adāsiyeva atthikassa;",
  "anya-e0601n0:105_0": "Dānapāramiṁ tato paraṁ apūri sīlapāramādikampi,",
  "anya-e0601n0:106_0": "Tāsamiddhiyopayātamaggataṁ tamekadīpakaṁ namāmi.",
  "anya-e0601n0:107_0": "22.",
  "anya-e0601n0:108_0": " Devādevātidevaṁ nidhanavapudharaṁ mārabhaṅgaṁ abhaṅgaṁ,",
  "anya-e0601n0:109_0": "Dīpaṁ dīpaṁ pajānaṁ jayavarasayane bodhipattaṁdhipattaṁ;",
  "anya-e0601n0:110_0": "Brahmābrahmāgatānaṁ varagirakathikaṁ pāpahīnaṁ pahīnaṁ,",
  "anya-e0601n0:111_0": "Lokālokābhirāmaṁ satatamabhiname taṁ munindaṁ munindaṁ.",
  "anya-e0601n0:112_0": "23.",
  "anya-e0601n0:113_0": " Buddho nigrodhabimbo mudukaracaraṇo brahmaghoseṇijaṅgho,",
  "anya-e0601n0:114_0": "Kosacchādaṅgajāto punarapi sugato suppatiṭṭhitapādo;",
  "anya-e0601n0:115_0": "Mūdodātuṇṇalomo athamapi sugato brahmujuggattabhāvo,",
  "anya-e0601n0:116_0": "Nīlakkhī dīghapaṇhī sukhumamalachavī thomyarasaggasaggī.",
  "anya-e0601n0:117_0": "24.",
  "anya-e0601n0:118_0": " Cattālīsaggadanto samakalapanajo antaraṁsappapīno,",
  "anya-e0601n0:119_0": "Cakkenaṅkitapādo aviraḷadasano mārajussaṅkhapādo.",
  "anya-e0601n0:120_0": "Tiṭṭhanto nonamantobhayakaramudunā jaṇṇukānāmasanto,",
  "anya-e0601n0:121_0": "Vaṭṭakkhandho jino gotaruṇapakhumako sīhapubbaḍḍhakāyo.",
  "anya-e0601n0:122_0": "25.",
  "anya-e0601n0:123_0": " Sattappīno ca dīghaṅguli matha sugato lomakūpekalomo,",
  "anya-e0601n0:124_0": "Sampannodātadāṭho kanakasamataco nīlamuddhaggalomo.",
  "anya-e0601n0:125_0": "Sambuddho thūlajivho atha sīhahanuko jālikappādahattho,",
  "anya-e0601n0:126_0": "Nātho uṇhīsasīso itiguṇasahitaṁ taṁ mahesiṁ namāmi.",
  "anya-e0601n0:127_0": "26.",
  "anya-e0601n0:128_0": " Buddhobuddhotighoso atidulabhataro kā kathā buddhabhāvo,",
  "anya-e0601n0:129_0": "Loke tasmā vibhāvī vividhahitasukhaṁ sādhavo patthayantā.",
  "anya-e0601n0:130_0": "Iṭṭhaṁ atthaṁ vahantaṁ suranaramahitaṁ nibbhayaṁ dakkhiṇeyyaṁ,",
  "anya-e0601n0:131_0": "Lokānaṁ nandivaḍḍhaṁ dasabalamasamaṁ taṁ namassantu niccaṁ.",
  "anya-e0601n0:132_0": "27.",
  "anya-e0601n0:133_0": " Puññenetena sohaṁ nipuṇamati sato samparāye ca titto,",
  "anya-e0601n0:134_0": "Dakkho diṭṭhujjupañño avikalavīriyo bhogavā saṁvibhāgī.",
  "anya-e0601n0:135_0": "Tikkho sūro dhitatto saparahitacaro dīghajīvī arogo,",
  "anya-e0601n0:136_0": "Dhañño vaṇṇo yasassī atibalavadharo kittimā khantupeto.",
  "anya-e0601n0:137_0": "28.",
  "anya-e0601n0:138_0": " Saddho dātaṅgupeto paramasiridharo diṭṭhadhamme viratto,",
  "anya-e0601n0:139_0": "Lajjī kalyāṇamitto abhiratakusalo pañcasīlādirakkho.",
  "anya-e0601n0:140_0": "Appiccho appakodho ativujuhadayo iddhimā appameyyo,",
  "anya-e0601n0:141_0": "Pāsaṁso pemavāco sujanaguṇavidū māmako so bhaveyyaṁ.",
  "anya-e0601n0:142_0": "29.",
  "anya-e0601n0:143_0": " Itthaṁ asaṅkhaye nātha, guṇe lakkhaṇadīpite;",
  "anya-e0601n0:144_0": "Gāthāsu sūcakāsveka, gāthampi sarate budho.",
  "anya-e0601n0:145_0": "30.",
  "anya-e0601n0:146_0": " Caturāpāyamutto so, sādhakatthadvayassa ca;",
  "anya-e0601n0:147_0": "Hatūpaddavajālo ca, lābhī hitasukhassa ca.",
  "anya-e0601n0:148_0": "31.",
  "anya-e0601n0:149_0": " Adhipo naradevānaṁ, catudīpissaropi vā;",
  "anya-e0601n0:150_0": "Bhaveyya antime dehe, tamaññaṁ setachattakaṁ.",
  "anya-e0601n0:151_0": "32.",
  "anya-e0601n0:152_0": " Bhāvanāyānamāruyha, samamessati subbato;",
  "anya-e0601n0:153_0": "Imasmiṁ attabhāvepi, arogo dīghajīviko.",
  "anya-e0601n0:154_0": "33.",
  "anya-e0601n0:155_0": " Pūjito sabbalokehi, bhāvanābhiratīmano;",
  "anya-e0601n0:156_0": "Janappiyo manāpo ca, kā kathākhiladhāraṇe.",
  "anya-e0601n0:157_0": "Namakkārapāḷi niṭṭhitā."
}