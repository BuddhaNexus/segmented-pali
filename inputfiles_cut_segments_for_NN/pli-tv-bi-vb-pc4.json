{
  "pli-tv-bi-vb-pc4:0.1_0": "Theravāda Vinaya",
  "pli-tv-bi-vb-pc4:0.2_0": "Bhikkhunivibhaṅga",
  "pli-tv-bi-vb-pc4:0.3_0": "Pācittiyakaṇḍa",
  "pli-tv-bi-vb-pc4:0.4_0": "Lasuṇavagga",
  "pli-tv-bi-vb-pc4:0.5_0": "4. Jatumaṭṭhakasikkhāpada",
  "pli-tv-bi-vb-pc4:1.1_0": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bi-vb-pc4:1.2_0": "Tena kho pana samayena aññatarā purāṇarājorodhā bhikkhunīsu pabbajitā hoti.",
  "pli-tv-bi-vb-pc4:1.3_0": "Aññatarā bhikkhunī anabhiratiyā pīḷitā yena sā bhikkhunī tenupasaṅkami;",
  "pli-tv-bi-vb-pc4:1.3_1": "upasaṅkamitvā taṁ bhikkhuniṁ etadavoca—",
  "pli-tv-bi-vb-pc4:1.4_0": "“rājā kho, ayye, tumhe cirāciraṁ gacchati.",
  "pli-tv-bi-vb-pc4:1.5_0": "Kathaṁ tumhe dhārethā”ti?",
  "pli-tv-bi-vb-pc4:1.6_0": "“Jatumaṭṭhakena, ayye”ti.",
  "pli-tv-bi-vb-pc4:1.7_0": "“Kiṁ etaṁ, ayye, jatumaṭṭhakan”ti?",
  "pli-tv-bi-vb-pc4:1.8_0": "Atha kho sā bhikkhunī tassā bhikkhuniyā jatumaṭṭhakaṁ ācikkhi.",
  "pli-tv-bi-vb-pc4:1.9_0": "Atha kho sā bhikkhunī jatumaṭṭhakaṁ ādiyitvā dhovituṁ vissaritvā ekamantaṁ chaḍḍesi.",
  "pli-tv-bi-vb-pc4:1.10_0": "Bhikkhuniyo makkhikāhi samparikiṇṇaṁ passitvā evamāhaṁsu—",
  "pli-tv-bi-vb-pc4:1.11_0": "“kassidaṁ kamman”ti?",
  "pli-tv-bi-vb-pc4:1.12_0": "Sā evamāha— “mayhidaṁ kamman”ti.",
  "pli-tv-bi-vb-pc4:1.13_0": "Yā tā bhikkhuniyo appicchā …pe… tā ujjhāyanti khiyyanti vipācenti—",
  "pli-tv-bi-vb-pc4:1.14_0": "“kathañhi nāma bhikkhunī jatumaṭṭhakaṁ ādiyissatī”ti …pe…",
  "pli-tv-bi-vb-pc4:1.15_0": "“saccaṁ kira, bhikkhave,",
  "pli-tv-bi-vb-pc4:1.15_1": "bhikkhunī jatumaṭṭhakaṁ ādiyatī”ti?",
  "pli-tv-bi-vb-pc4:1.16_0": "“Saccaṁ, bhagavā”ti.",
  "pli-tv-bi-vb-pc4:1.17_0": "Vigarahi buddho bhagavā …pe…",
  "pli-tv-bi-vb-pc4:1.18_0": "kathañhi nāma, bhikkhave,",
  "pli-tv-bi-vb-pc4:1.18_1": "bhikkhunī jatumaṭṭhakaṁ ādiyissati.",
  "pli-tv-bi-vb-pc4:1.19_0": "Netaṁ, bhikkhave, appasannānaṁ vā pasādāya …pe…",
  "pli-tv-bi-vb-pc4:1.20_0": "evañca pana, bhikkhave,",
  "pli-tv-bi-vb-pc4:1.20_1": "bhikkhuniyo imaṁ sikkhāpadaṁ uddisantu—",
  "pli-tv-bi-vb-pc4:1.21.1_0": "“Jatumaṭṭhake pācittiyan”ti.",
  "pli-tv-bi-vb-pc4:2.1_0": "Jatumaṭṭhakaṁ nāma",
  "pli-tv-bi-vb-pc4:2.2_0": "jatumayaṁ kaṭṭhamayaṁ piṭṭhamayaṁ mattikāmayaṁ.",
  "pli-tv-bi-vb-pc4:2.3_0": "Ādiyeyyāti",
  "pli-tv-bi-vb-pc4:2.4_0": "samphassaṁ sādiyantī antamaso uppalapattampi muttakaraṇaṁ paveseti, āpatti pācittiyassa.",
  "pli-tv-bi-vb-pc4:2.5.1_0": "Anāpatti—",
  "pli-tv-bi-vb-pc4:2.6_0": "ābādhapaccayā,",
  "pli-tv-bi-vb-pc4:2.7_0": "ummattikāya,",
  "pli-tv-bi-vb-pc4:2.8_0": "ādikammikāyāti.",
  "pli-tv-bi-vb-pc4:2.9_0": "Catutthasikkhāpadaṁ niṭṭhitaṁ."
}