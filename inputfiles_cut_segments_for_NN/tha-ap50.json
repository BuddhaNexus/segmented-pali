{
  "tha-ap50:0.1_0": "Therāpadāna",
  "tha-ap50:0.2_0": "Upālivagga",
  "tha-ap50:0.3_0": "8 Caṅkamanadāyakattheraapadāna",
  "tha-ap50:1.1_0": "“Atthadassissa munino,",
  "tha-ap50:1.2_0": "lokajeṭṭhassa tādino;",
  "tha-ap50:1.3_0": "Iṭṭhakāhi cinitvāna,",
  "tha-ap50:1.4_0": "caṅkamaṁ kārayiṁ ahaṁ.",
  "tha-ap50:2.1_0": "Uccato pañcaratanaṁ,",
  "tha-ap50:2.2_0": "caṅkamaṁ sādhumāpitaṁ;",
  "tha-ap50:2.3_0": "Āyāmato hatthasataṁ,",
  "tha-ap50:2.4_0": "bhāvanīyyaṁ manoramaṁ.",
  "tha-ap50:3.1_0": "Paṭiggahesi bhagavā,",
  "tha-ap50:3.2_0": "atthadassī naruttamo;",
  "tha-ap50:3.3_0": "Hatthena pulinaṁ gayha,",
  "tha-ap50:3.4_0": "imā gāthā abhāsatha.",
  "tha-ap50:4.1_0": "‘Iminā pulinadānena,",
  "tha-ap50:4.2_0": "caṅkamaṁ sukatena ca;",
  "tha-ap50:4.3_0": "Sattaratanasampannaṁ,",
  "tha-ap50:4.4_0": "pulinaṁ anubhossati.",
  "tha-ap50:5.1_0": "Tīṇi kappāni devesu,",
  "tha-ap50:5.2_0": "devarajjaṁ karissati;",
  "tha-ap50:5.3_0": "Anubhossati sampattiṁ,",
  "tha-ap50:5.4_0": "accharāhi purakkhato.",
  "tha-ap50:6.1_0": "Manussalokamāgantvā,",
  "tha-ap50:6.2_0": "rājā raṭṭhe bhavissati;",
  "tha-ap50:6.3_0": "Tikkhattuṁ cakkavattī ca,",
  "tha-ap50:6.4_0": "mahiyā so bhavissati’.",
  "tha-ap50:7.1_0": "Aṭṭhārase kappasate,",
  "tha-ap50:7.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap50:7.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap50:7.4_0": "caṅkamassa idaṁ phalaṁ.",
  "tha-ap50:8.1_0": "Paṭisambhidā catasso,",
  "tha-ap50:8.2_0": "…pe…",
  "tha-ap50:8.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap50:9.1_0": "Itthaṁ sudaṁ āyasmā caṅkamanadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap50:10.1_0": "Caṅkamanadāyakattherassāpadānaṁ aṭṭhamaṁ."
}