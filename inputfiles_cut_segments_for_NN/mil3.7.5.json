{
  "mil3.7.5:0.1_0": "Milindapañha",
  "mil3.7.5:0.2_0": "Arūpadhammavavatthānavagga",
  "mil3.7.5:0.3_0": "5. Dvinnaṁlokuppannānaṁsamakabhāvapañha",
  "mil3.7.5:1.1_0": "Rājā āha—",
  "mil3.7.5:1.2_0": "“bhante nāgasena,",
  "mil3.7.5:1.2_1": "yo idha kālaṅkato brahmaloke uppajjeyya,",
  "mil3.7.5:1.2_2": "yo ca idha kālaṅkato kasmīre uppajjeyya,",
  "mil3.7.5:1.2_3": "ko cirataraṁ ko sīghataran”ti?",
  "mil3.7.5:1.3_0": "“Samakaṁ, mahārājā”ti.",
  "mil3.7.5:2.1_0": "“Opammaṁ karohī”ti.",
  "mil3.7.5:2.2_0": "“Kuhiṁ pana, mahārāja, tava jātanagaran”ti?",
  "mil3.7.5:2.3_0": "“Atthi, bhante, kalasigāmo nāma, tatthāhaṁ jāto”ti.",
  "mil3.7.5:2.4_0": "“Kīva dūro, mahārāja, ito kalasigāmo hotī”ti.",
  "mil3.7.5:2.5_0": "“Dvimattāni, bhante, yojanasatānī”ti.",
  "mil3.7.5:2.6_0": "“Kīva dūraṁ, mahārāja, ito kasmīraṁ hotī”ti?",
  "mil3.7.5:2.7_0": "“Dvādasa, bhante, yojanānī”ti.",
  "mil3.7.5:2.8_0": "“Iṅgha tvaṁ, mahārāja, kalasigāmaṁ cintehī”ti.",
  "mil3.7.5:2.9_0": "“Cintito, bhante”ti.",
  "mil3.7.5:2.10_0": "“Iṅgha tvaṁ, mahārāja, kasmīraṁ cintehī”ti.",
  "mil3.7.5:2.11_0": "“Cintitaṁ, bhante”ti.",
  "mil3.7.5:2.12_0": "“Katamaṁ nu kho, mahārāja, cirena cintitaṁ, katamaṁ sīghataran”ti?",
  "mil3.7.5:2.13_0": "“Samakaṁ, bhante”ti.",
  "mil3.7.5:2.14_0": "“Evameva kho, mahārāja,",
  "mil3.7.5:2.14_1": "yo idha kālaṅkato brahmaloke uppajjeyya,",
  "mil3.7.5:2.14_2": "yo ca idha kālaṅkato kasmīre uppajjeyya, samakaṁyeva uppajjantī”ti.",
  "mil3.7.5:3.1_0": "“Bhiyyo opammaṁ karohī”ti.",
  "mil3.7.5:3.2_0": "“Taṁ kiṁ maññasi, mahārāja,",
  "mil3.7.5:3.2_1": "dve sakuṇā ākāsena gaccheyyuṁ,",
  "mil3.7.5:3.2_2": "tesu eko ucce rukkhe nisīdeyya, eko nīce rukkhe nisīdeyya,",
  "mil3.7.5:3.2_3": "tesaṁ samakaṁ patiṭṭhitānaṁ katamassa chāyā paṭhamataraṁ pathaviyaṁ patiṭṭhaheyya,",
  "mil3.7.5:3.2_4": "katamassa chāyā cirena pathaviyaṁ patiṭṭhaheyyā”ti?",
  "mil3.7.5:3.3_0": "“Samakaṁ, bhante”ti.",
  "mil3.7.5:3.4_0": "“Evameva kho, mahārāja,",
  "mil3.7.5:3.4_1": "yo idha kālaṅkato brahmaloke uppajjeyya,",
  "mil3.7.5:3.4_2": "yo ca idha kālaṅkato kasmīre uppajjeyya, samakaṁyeva uppajjantī”ti.",
  "mil3.7.5:4.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.7.5:5.1_0": "Dvinnaṁ lokuppannānaṁ samakabhāvapañho pañcamo."
}