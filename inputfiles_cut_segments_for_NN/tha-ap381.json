{
  "tha-ap381:0.1_0": "Therāpadāna",
  "tha-ap381:0.2_0": "Bodhivandanavagga",
  "tha-ap381:0.3_0": "9. Sumanadāmiyattheraapadāna",
  "tha-ap381:1.1_0": "“Siddhatthassa bhagavato,",
  "tha-ap381:1.2_0": "nhātakassa tapassino;",
  "tha-ap381:1.3_0": "Katvāna sumanadāmaṁ,",
  "tha-ap381:1.4_0": "dhārayiṁ purato ṭhito.",
  "tha-ap381:2.1_0": "Catunnavutito kappe,",
  "tha-ap381:2.2_0": "yaṁ dāmaṁ dhārayiṁ tadā;",
  "tha-ap381:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap381:2.4_0": "sumanadhāraṇe phalaṁ.",
  "tha-ap381:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap381:3.2_0": "…pe…",
  "tha-ap381:3.3_0": "viharāmi anāsavo.",
  "tha-ap381:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap381:4.2_0": "…pe…",
  "tha-ap381:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap381:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap381:5.2_0": "…pe…",
  "tha-ap381:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap381:6.1_0": "Itthaṁ sudaṁ āyasmā sumanadāmiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap381:7.1_0": "Sumanadāmiyattherassāpadānaṁ navamaṁ."
}