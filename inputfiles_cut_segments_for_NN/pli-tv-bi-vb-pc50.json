{
  "pli-tv-bi-vb-pc50:0.1_0": "Theravāda Vinaya",
  "pli-tv-bi-vb-pc50:0.2_0": "Bhikkhunivibhaṅga",
  "pli-tv-bi-vb-pc50:0.3_0": "Pācittiyakaṇḍa",
  "pli-tv-bi-vb-pc50:0.4_0": "Cittāgāravagga",
  "pli-tv-bi-vb-pc50:0.5_0": "50. Tiracchānavijjāvācanasikkhāpada",
  "pli-tv-bi-vb-pc50:1.1_0": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bi-vb-pc50:1.2_0": "Tena kho pana samayena chabbaggiyā bhikkhuniyo tiracchānavijjaṁ vācenti.",
  "pli-tv-bi-vb-pc50:1.3_0": "Manussā ujjhāyanti khiyyanti vipācenti—",
  "pli-tv-bi-vb-pc50:1.4_0": "“kathañhi nāma bhikkhuniyo tiracchānavijjaṁ vācessanti,",
  "pli-tv-bi-vb-pc50:1.4_1": "seyyathāpi gihiniyo kāmabhoginiyo”ti.",
  "pli-tv-bi-vb-pc50:1.5_0": "Assosuṁ kho bhikkhuniyo tesaṁ manussānaṁ ujjhāyantānaṁ khiyyantānaṁ vipācentānaṁ.",
  "pli-tv-bi-vb-pc50:1.6_0": "Yā tā bhikkhuniyo appicchā …pe… tā ujjhāyanti khiyyanti vipācenti—",
  "pli-tv-bi-vb-pc50:1.7_0": "“kathañhi nāma chabbaggiyā bhikkhuniyo tiracchānavijjaṁ vācessantī”ti …pe…",
  "pli-tv-bi-vb-pc50:1.8_0": "“saccaṁ kira, bhikkhave,",
  "pli-tv-bi-vb-pc50:1.8_1": "chabbaggiyā bhikkhuniyo tiracchānavijjaṁ vācentī”ti?",
  "pli-tv-bi-vb-pc50:1.9_0": "“Saccaṁ, bhagavā”ti.",
  "pli-tv-bi-vb-pc50:1.10_0": "Vigarahi buddho bhagavā …pe…",
  "pli-tv-bi-vb-pc50:1.11_0": "kathañhi nāma, bhikkhave,",
  "pli-tv-bi-vb-pc50:1.11_1": "chabbaggiyā bhikkhuniyo tiracchānavijjaṁ vācessanti.",
  "pli-tv-bi-vb-pc50:1.12_0": "Netaṁ, bhikkhave, appasannānaṁ vā pasādāya …pe…",
  "pli-tv-bi-vb-pc50:1.13_0": "evañca pana, bhikkhave,",
  "pli-tv-bi-vb-pc50:1.13_1": "bhikkhuniyo imaṁ sikkhāpadaṁ uddisantu—",
  "pli-tv-bi-vb-pc50:1.14.1_0": "“Yā pana bhikkhunī tiracchānavijjaṁ vāceyya, pācittiyan”ti.",
  "pli-tv-bi-vb-pc50:2.1_0": "Yā panāti",
  "pli-tv-bi-vb-pc50:2.2_0": "yā yādisā …pe…",
  "pli-tv-bi-vb-pc50:2.3_0": "bhikkhunīti",
  "pli-tv-bi-vb-pc50:2.4_0": "…pe… ayaṁ imasmiṁ atthe adhippetā bhikkhunīti.",
  "pli-tv-bi-vb-pc50:2.5_0": "Tiracchānavijjā nāma",
  "pli-tv-bi-vb-pc50:2.6_0": "yaṁ kiñci bāhirakaṁ anatthasaṁhitaṁ.",
  "pli-tv-bi-vb-pc50:2.7_0": "Vāceyyāti",
  "pli-tv-bi-vb-pc50:2.8_0": "padena vāceti, pade pade āpatti pācittiyassa.",
  "pli-tv-bi-vb-pc50:2.9_0": "Akkharāya vāceti,",
  "pli-tv-bi-vb-pc50:2.9_1": "akkharakkharāya āpatti pācittiyassa.",
  "pli-tv-bi-vb-pc50:2.10.1_0": "Anāpatti—",
  "pli-tv-bi-vb-pc50:2.11_0": "lekhaṁ vāceti,",
  "pli-tv-bi-vb-pc50:2.12_0": "dhāraṇaṁ vāceti,",
  "pli-tv-bi-vb-pc50:2.13_0": "guttatthāya parittaṁ vāceti,",
  "pli-tv-bi-vb-pc50:2.14_0": "ummattikāya,",
  "pli-tv-bi-vb-pc50:2.15_0": "ādikammikāyāti.",
  "pli-tv-bi-vb-pc50:2.16_0": "Dasamasikkhāpadaṁ niṭṭhitaṁ.",
  "pli-tv-bi-vb-pc50:2.17_0": "Cittāgāravaggo pañcamo."
}