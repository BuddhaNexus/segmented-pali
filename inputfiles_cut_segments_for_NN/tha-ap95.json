{
  "tha-ap95:0.1_0": "Therāpadāna",
  "tha-ap95:0.2_0": "Sudhāvagga",
  "tha-ap95:0.3_0": "3 Aḍḍhaceḷakattheraapadāna",
  "tha-ap95:1.1_0": "“Tissassāhaṁ bhagavato,",
  "tha-ap95:1.2_0": "upaḍḍhadussamadāsahaṁ;",
  "tha-ap95:1.3_0": "Paramakāpaññapattomhi,",
  "tha-ap95:1.4_0": "duggatena samappito.",
  "tha-ap95:2.1_0": "Upaḍḍhadussaṁ datvāna,",
  "tha-ap95:2.2_0": "kappaṁ saggamhi modahaṁ;",
  "tha-ap95:2.3_0": "Avasesesu kappesu,",
  "tha-ap95:2.4_0": "kusalaṁ kāritaṁ mayā.",
  "tha-ap95:3.1_0": "Dvenavute ito kappe,",
  "tha-ap95:3.2_0": "yaṁ dussamadadiṁ tadā;",
  "tha-ap95:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap95:3.4_0": "dussadānassidaṁ phalaṁ.",
  "tha-ap95:4.1_0": "Ekūnapaññāsakappamhi,",
  "tha-ap95:4.2_0": "rājāno cakkavattino;",
  "tha-ap95:4.3_0": "Samantacchadanā nāma,",
  "tha-ap95:4.4_0": "bāttiṁsāsuṁ janādhipā.",
  "tha-ap95:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap95:5.2_0": "…pe…",
  "tha-ap95:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap95:6.1_0": "Itthaṁ sudaṁ āyasmā aḍḍhaceḷako thero imā gāthāyo abhāsitthāti.",
  "tha-ap95:7.1_0": "Aḍḍhaceḷakattherassāpadānaṁ tatiyaṁ."
}