{
  "patthana18.10:0.1_0": "Paṭṭhānapakaraṇa",
  "patthana18.10:0.2_0": "Dhammānulomapaccanīya (3), Dukadukapaṭṭhānapāḷi (6)",
  "patthana18.10:0.3_0": "18.10. Sahetukaduka, Hetuduka",
  "patthana18.10:1.1_0": "Sahetukaṁ hetuṁ dhammaṁ paṭicca nasahetuko nahetu dhammo uppajjati hetupaccayā … tīṇi.",
  "patthana18.10:2.1_0": "Ahetukaṁ hetuṁ dhammaṁ paṭicca naahetuko nahetu dhammo uppajjati hetupaccayā … tīṇi.",
  "patthana18.10:2.1_1": "(Saṅkhittaṁ.) Hetuyā cha,",
  "patthana18.10:2.1_2": "ārammaṇe tīṇi …pe…",
  "patthana18.10:2.1_3": "aññamaññe cattāri …pe…",
  "patthana18.10:2.1_4": "vipāke tīṇi …pe…",
  "patthana18.10:2.1_5": "avigate cha.",
  "patthana18.10:3.1_0": "Sahetukaṁ nahetuṁ dhammaṁ paṭicca naahetuko nanahetu dhammo uppajjati hetupaccayā.",
  "patthana18.10:3.1_1": "(Saṅkhittaṁ.) Hetuyā tīṇi,",
  "patthana18.10:3.1_2": "ārammaṇe pañca,",
  "patthana18.10:3.1_3": "adhipatiyā ekaṁ …pe…",
  "patthana18.10:3.1_4": "sahajāte aññamaññe pañca …pe…",
  "patthana18.10:3.1_5": "purejāte āsevane tīṇi …pe…",
  "patthana18.10:3.1_6": "vipāke tīṇi …pe…",
  "patthana18.10:3.1_7": "avigate pañca."
}