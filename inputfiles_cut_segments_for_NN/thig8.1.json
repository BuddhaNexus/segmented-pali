{
  "thig8.1:0.1_0": "Therīgāthā",
  "thig8.1:0.2_0": "Aṭṭhakanipāta",
  "thig8.1:0.3_0": "1. Sīsūpacālātherīgāthā",
  "thig8.1:1.1_0": "“Bhikkhunī sīlasampannā,",
  "thig8.1:1.2_0": "indriyesu susaṁvutā;",
  "thig8.1:1.3_0": "Adhigacche padaṁ santaṁ,",
  "thig8.1:1.4_0": "asecanakamojavaṁ”.",
  "thig8.1:2.1_0": "“Tāvatiṁsā ca yāmā ca,",
  "thig8.1:2.2_0": "tusitā cāpi devatā;",
  "thig8.1:2.3_0": "Nimmānaratino devā,",
  "thig8.1:2.4_0": "ye devā vasavattino;",
  "thig8.1:2.5_0": "Tattha cittaṁ paṇīdhehi,",
  "thig8.1:2.6_0": "yattha te vusitaṁ pure”.",
  "thig8.1:3.1_0": "“Tāvatiṁsā ca yāmā ca,",
  "thig8.1:3.2_0": "tusitā cāpi devatā;",
  "thig8.1:3.3_0": "Nimmānaratino devā,",
  "thig8.1:3.4_0": "ye devā vasavattino.",
  "thig8.1:4.1_0": "Kālaṁ kālaṁ bhavābhavaṁ,",
  "thig8.1:4.2_0": "sakkāyasmiṁ purakkhatā;",
  "thig8.1:4.3_0": "Avītivattā sakkāyaṁ,",
  "thig8.1:4.4_0": "jātimaraṇasārino.",
  "thig8.1:5.1_0": "Sabbo ādīpito loko,",
  "thig8.1:5.2_0": "sabbo loko padīpito;",
  "thig8.1:5.3_0": "Sabbo pajjalito loko,",
  "thig8.1:5.4_0": "sabbo loko pakampito.",
  "thig8.1:6.1_0": "Akampiyaṁ atuliyaṁ,",
  "thig8.1:6.2_0": "aputhujjanasevitaṁ;",
  "thig8.1:6.3_0": "Buddho dhammamadesesi,",
  "thig8.1:6.4_0": "tattha me nirato mano.",
  "thig8.1:7.1_0": "Tassāhaṁ vacanaṁ sutvā,",
  "thig8.1:7.2_0": "vihariṁ sāsane ratā;",
  "thig8.1:7.3_0": "Tisso vijjā anuppattā,",
  "thig8.1:7.4_0": "kataṁ buddhassa sāsanaṁ.",
  "thig8.1:8.1_0": "Sabbattha vihatā nandī,",
  "thig8.1:8.2_0": "tamokhandho padālito;",
  "thig8.1:8.3_0": "Evaṁ jānāhi pāpima,",
  "thig8.1:8.4_0": "nihato tvamasi antaka”.",
  "thig8.1:9.1_0": "…",
  "thig8.1:9.2_0": "Sīsūpacālā therī ….",
  "thig8.1:10.1_0": "Aṭṭhakanipāto niṭṭhito."
}