{
  "iti102:0.1_0": "Itivuttaka 102",
  "iti102:0.2_0": "Catukkanipāta",
  "iti102:0.3_0": "Brāhmaṇadhammayāgavagga",
  "iti102:0.4_0": "Āsavakkhayasutta",
  "iti102:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti102:2.1_0": "“Jānatohaṁ, bhikkhave, passato āsavānaṁ khayaṁ vadāmi, no ajānato no apassato.",
  "iti102:2.2_0": "Kiñca, bhikkhave, jānato, kiṁ passato āsavānaṁ khayo hoti?",
  "iti102:2.3_0": "Idaṁ dukkhanti,",
  "iti102:2.3_1": "bhikkhave,",
  "iti102:2.3_2": "jānato passato āsavānaṁ khayo hoti.",
  "iti102:2.3_3": "Ayaṁ dukkhasamudayoti,",
  "iti102:2.3_4": "bhikkhave,",
  "iti102:2.3_5": "jānato passato āsavānaṁ khayo hoti.",
  "iti102:2.3_6": "Ayaṁ dukkhanirodhoti,",
  "iti102:2.3_7": "bhikkhave,",
  "iti102:2.3_8": "jānato passato āsavānaṁ khayo hoti.",
  "iti102:2.3_9": "Ayaṁ dukkhanirodhagāminī paṭipadāti,",
  "iti102:2.3_10": "bhikkhave,",
  "iti102:2.3_11": "jānato passato āsavānaṁ khayo hoti.",
  "iti102:2.4_0": "Evaṁ kho, bhikkhave, jānato evaṁ passato āsavānaṁ khayo hotī”ti.",
  "iti102:2.5_0": "Etamatthaṁ bhagavā avoca.",
  "iti102:2.6_0": "Tatthetaṁ iti vuccati:",
  "iti102:3.1_0": "“Sekhassa sikkhamānassa,",
  "iti102:3.2_0": "ujumaggānusārino;",
  "iti102:3.3_0": "Khayasmiṁ paṭhamaṁ ñāṇaṁ,",
  "iti102:3.4_0": "tato aññā anantarā.",
  "iti102:4.1_0": "Tato aññāvimuttassa,",
  "iti102:4.2_0": "vimuttiñāṇamuttamaṁ;",
  "iti102:4.3_0": "Uppajjati khaye ñāṇaṁ,",
  "iti102:4.4_0": "khīṇā saṁyojanā iti.",
  "iti102:5.1_0": "Na tvevidaṁ kusītena,",
  "iti102:5.2_0": "bālenamavijānatā;",
  "iti102:5.3_0": "Nibbānaṁ adhigantabbaṁ,",
  "iti102:5.4_0": "sabbaganthappamocanan”ti.",
  "iti102:6.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti102:6.2_0": "Tatiyaṁ."
}