{
  "anya-e1005n4:0_0": "5. Itthikaṇḍo",
  "anya-e1005n4:1_0": "94.",
  "anya-e1005n4:2_0": " Kokilānaṁ saddaṁ rūpaṁ,",
  "anya-e1005n4:3_0": "Nārīrūpaṁ patibbatā;",
  "anya-e1005n4:4_0": "Vijjā rūpaṁ a-rūpānaṁ,",
  "anya-e1005n4:5_0": "Khamā rūpaṁ tapassinaṁ.",
  "anya-e1005n4:6_0": "95.",
  "anya-e1005n4:7_0": " Itthīnañca dhanaṁ rūpaṁ,",
  "anya-e1005n4:8_0": "Purisānaṁ vijjā dhanaṁ;",
  "anya-e1005n4:9_0": "Bhikkhūnañca dhanaṁ sīlaṁ,",
  "anya-e1005n4:10_0": "Rājānañca dhanaṁ balaṁ.",
  "anya-e1005n4:11_0": "96.",
  "anya-e1005n4:12_0": " Tapassino kisā sobhā,",
  "anya-e1005n4:13_0": "Thūlā sobhā catuppadā;",
  "anya-e1005n4:14_0": "Purisā vijjavā sobhā,",
  "anya-e1005n4:15_0": "Itthī sobhāsa sāmikā.",
  "anya-e1005n4:16_0": "97.",
  "anya-e1005n4:17_0": " Pañca ratyā sugandhabbā,",
  "anya-e1005n4:18_0": "Satta ratyā dhanuggahā;",
  "anya-e1005n4:19_0": "Eka māsā subhariyā,",
  "anya-e1005n4:20_0": "Aḍḍha māsā sissā malā.",
  "anya-e1005n4:21_0": "98.",
  "anya-e1005n4:22_0": " Hiṁ ramati paṅa,",
  "anya-e1005n4:23_0": "Haṅa ramati poka.",
  "anya-e1005n4:24_0": "Thī ramati pu,",
  "anya-e1005n4:25_0": "Khu ramati dhaṁ.",
  "anya-e1005n4:26_0": "99.",
  "anya-e1005n4:27_0": " Jiṇṇamannaṁ pasaṁseyya,",
  "anya-e1005n4:28_0": "Dārañca gatayobbanaṁ;",
  "anya-e1005n4:29_0": "Raṇā punāgatā sūraṁ,",
  "anya-e1005n4:30_0": "Sassañca gehamāgataṁ.",
  "anya-e1005n4:31_0": "100.",
  "anya-e1005n4:32_0": " Dvatti-patikā nārī ca,",
  "anya-e1005n4:33_0": "Bhikkhu dvatti-vihāriko;",
  "anya-e1005n4:34_0": "Dvatti-pāsa-mutto pakkhī,",
  "anya-e1005n4:35_0": "Kata-māyā bahūtharaṁ.",
  "anya-e1005n4:36_0": "101.",
  "anya-e1005n4:37_0": " Dujjanaṁ pahārādame,",
  "anya-e1005n4:38_0": "Mittaṁ dame a-bhāṇikā;",
  "anya-e1005n4:39_0": "Itthiñca byasanā dame,",
  "anya-e1005n4:40_0": "Rāginaṁ appa bhojanā.",
  "anya-e1005n4:41_0": "102.",
  "anya-e1005n4:42_0": " Na ratti vinā candimā,",
  "anya-e1005n4:43_0": "Vīciṁ vinā ca aṇṇavo;",
  "anya-e1005n4:44_0": "Haṁsaṁ vinā pokkharaṇī,",
  "anya-e1005n4:45_0": "Patiṁ kaññāca sobhate.",
  "anya-e1005n4:46_0": "103.",
  "anya-e1005n4:47_0": " Patinā janito bhogo,",
  "anya-e1005n4:48_0": "Itthiyāva saṁgoppito;",
  "anya-e1005n4:49_0": "Purisova hi padhāno,",
  "anya-e1005n4:50_0": "Itthī suttaṁva sūciyā.",
  "anya-e1005n4:51_0": "104.",
  "anya-e1005n4:52_0": " Sabbānadī vaṅkanadī,",
  "anya-e1005n4:53_0": "Sabbe kaṭṭhamayā vanā;",
  "anya-e1005n4:54_0": "Sabbitthiyo kare pāpaṁ,",
  "anya-e1005n4:55_0": "Labhamāne nivātake.",
  "anya-e1005n4:56_0": "105.",
  "anya-e1005n4:57_0": " Vivādasīliṁ usūyabhāṇiniṁ,",
  "anya-e1005n4:58_0": "Sampassataṇhiṁ bahupākabhuttiniṁ;",
  "anya-e1005n4:59_0": "Aggantabhuttiṁ paragehavāsiniṁ,",
  "anya-e1005n4:60_0": "Nāriṁ caje puttasatampi pūmā.",
  "anya-e1005n4:61_0": "106.",
  "anya-e1005n4:62_0": " Bhuttesu maṇḍesu janīva kantinī,",
  "anya-e1005n4:63_0": "Guyheca ṭhāne bhaginīva hiriṇī;",
  "anya-e1005n4:64_0": "Kammesu pattesu karoti dāsīva,",
  "anya-e1005n4:65_0": "Bhayesu mantī sayanesu rāmaye;",
  "anya-e1005n4:66_0": "Rūpīsu sikkhī kupanesu khantinī,",
  "anya-e1005n4:67_0": "Sā nārī seṭṭhāti vadanti paṇḍitā;",
  "anya-e1005n4:68_0": "Kāyassa bhedāca divebhaveyya sā.",
  "anya-e1005n4:69_0": "107.",
  "anya-e1005n4:70_0": " Sāmā migakkhī tanumajjhagattā,",
  "anya-e1005n4:71_0": "Sūrū sukesī samadantapantī;",
  "anya-e1005n4:72_0": "Gambhīranābhī yuvatī susīlī,",
  "anya-e1005n4:73_0": "Hīne kule jātāpi vivāhyā.",
  "anya-e1005n4:74_0": "108.",
  "anya-e1005n4:75_0": " Saradaṁratu-kālānaṁ,",
  "anya-e1005n4:76_0": "Bhariyānaṁ rūpavatī;",
  "anya-e1005n4:77_0": "Jeṭṭho padhānaṁ puttānaṁ,",
  "anya-e1005n4:78_0": "Disānaṁ uttarādisā.",
  "anya-e1005n4:79_0": "109.",
  "anya-e1005n4:80_0": " Yā icche puriso hotuṁ,",
  "anya-e1005n4:81_0": "Jāti jāti punappunaṁ;",
  "anya-e1005n4:82_0": "Sāmikaṁ apacāyeyya,",
  "anya-e1005n4:83_0": "Indaṁva pāricārikā.",
  "anya-e1005n4:84_0": "110.",
  "anya-e1005n4:85_0": " Yo icche puriso hotuṁ,",
  "anya-e1005n4:86_0": "Jāti jāti punappunaṁ;",
  "anya-e1005n4:87_0": "Paradānaṁ vivajjeyya,",
  "anya-e1005n4:88_0": "Dhotapādova kaddamaṁ.",
  "anya-e1005n4:89_0": "111.",
  "anya-e1005n4:90_0": " Atikkanta vayo poso,",
  "anya-e1005n4:91_0": "Āneti timbaruttaniṁ;",
  "anya-e1005n4:92_0": "Tassā issā asaddhāti,",
  "anya-e1005n4:93_0": "Taṁ parābhavato mukhaṁ.",
  "anya-e1005n4:94_0": "Itthikaṇḍo niṭṭhito."
}