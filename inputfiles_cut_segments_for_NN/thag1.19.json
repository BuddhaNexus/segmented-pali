{
  "thag1.19:0.1_0": "Theragāthā",
  "thag1.19:0.2_0": "Ekakanipāta",
  "thag1.19:0.3_0": "Dutiyavagga",
  "thag1.19:0.4_0": "1.19 Kulattheragāthā",
  "thag1.19:1.1_0": "“Udakañhi nayanti nettikā,",
  "thag1.19:1.2_0": "Usukārā namayanti tejanaṁ;",
  "thag1.19:1.3_0": "Dāruṁ namayanti tacchakā,",
  "thag1.19:1.4_0": "Attānaṁ damayanti subbatā”ti.",
  "thag1.19:1.5_0": "… Kulo thero …."
}