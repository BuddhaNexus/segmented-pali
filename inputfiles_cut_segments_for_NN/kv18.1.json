{
  "kv18.1:0.1_0": "Kathāvatthu",
  "kv18.1:0.2_0": "Niggahapaṇṇāsaka",
  "kv18.1:0.3_0": "Aṭṭhārasamavagga",
  "kv18.1:0.4_0": "Manussalokakathā",
  "kv18.1:1.1_0": "Na vattabbaṁ—",
  "kv18.1:1.2_0": "“buddho bhagavā manussaloke aṭṭhāsī”ti?",
  "kv18.1:1.3_0": "Āmantā.",
  "kv18.1:1.4_0": "Nanu atthi buddhavutthāni cetiyāni ārāmavihāragāmanigamanagarāni raṭṭhāni janapadānīti?",
  "kv18.1:1.5_0": "Āmantā.",
  "kv18.1:1.6_0": "Hañci atthi buddhavutthāni cetiyāni ārāmavihāragāmanigamanagarāni raṭṭhāni janapadāni, tena vata re vattabbe—",
  "kv18.1:1.7_0": "“buddho bhagavā manussaloke aṭṭhāsī”ti.",
  "kv18.1:2.1_0": "Na vattabbaṁ—",
  "kv18.1:2.2_0": "“buddho bhagavā manussaloke aṭṭhāsī”ti?",
  "kv18.1:2.3_0": "Āmantā.",
  "kv18.1:2.4_0": "Nanu bhagavā lumbiniyā jāto, bodhiyā mūle abhisambuddho,",
  "kv18.1:2.4_1": "bārāṇasiyaṁ bhagavatā dhammacakkaṁ pavattitaṁ,",
  "kv18.1:2.4_2": "cāpāle cetiye āyusaṅkhāro ossaṭṭho,",
  "kv18.1:2.4_3": "kusinārāyaṁ bhagavā parinibbutoti?",
  "kv18.1:2.5_0": "Āmantā.",
  "kv18.1:2.6_0": "Hañci bhagavā lumbiniyā jāto …pe… kusinārāyaṁ bhagavā parinibbuto, tena vata re vattabbe—",
  "kv18.1:2.7_0": "“buddho bhagavā manussaloke aṭṭhāsī”ti.",
  "kv18.1:3.1_0": "Na vattabbaṁ—",
  "kv18.1:3.2_0": "“buddho bhagavā manussaloke aṭṭhāsī”ti?",
  "kv18.1:3.3_0": "Āmantā.",
  "kv18.1:3.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv18.1:3.5_0": "“ekamidāhaṁ, bhikkhave,",
  "kv18.1:3.5_1": "samayaṁ ukkaṭṭhāyaṁ viharāmi subhagavane sālarājamūle”ti;",
  "kv18.1:3.6_0": "“ekamidāhaṁ, bhikkhave,",
  "kv18.1:3.6_1": "samayaṁ uruvelāyaṁ viharāmi ajapālanigrodhe paṭhamābhisambuddho”ti;",
  "kv18.1:3.7_0": "“ekamidāhaṁ, bhikkhave,",
  "kv18.1:3.7_1": "samayaṁ rājagahe viharāmi veḷuvane kalandakanivāpe”ti;",
  "kv18.1:3.8_0": "“ekamidāhaṁ, bhikkhave,",
  "kv18.1:3.8_1": "samayaṁ sāvatthiyaṁ viharāmi jetavane anāthapiṇḍikassa ārāme”ti;",
  "kv18.1:3.9_0": "“ekamidāhaṁ, bhikkhave,",
  "kv18.1:3.9_1": "samayaṁ vesāliyaṁ viharāmi mahāvane kūṭāgārasālāyan”ti.",
  "kv18.1:3.10_0": "Attheva suttantoti?",
  "kv18.1:3.11_0": "Āmantā.",
  "kv18.1:3.12_0": "Tena hi buddho bhagavā manussaloke aṭṭhāsīti.",
  "kv18.1:4.1_0": "Buddho bhagavā manussaloke aṭṭhāsīti?",
  "kv18.1:4.2_0": "Āmantā.",
  "kv18.1:4.3_0": "Nanu bhagavā loke jāto, loke saṁvaḍḍho,",
  "kv18.1:4.3_1": "lokaṁ abhibhuyya viharati anupalitto lokenāti?",
  "kv18.1:4.4_0": "Āmantā.",
  "kv18.1:4.5_0": "Hañci bhagavā loke jāto, loke saṁvaḍḍho,",
  "kv18.1:4.5_1": "lokaṁ abhibhuyya viharati anupalitto lokena, no ca vata re vattabbe—",
  "kv18.1:4.6_0": "“buddho bhagavā manussaloke aṭṭhāsī”ti.",
  "kv18.1:4.7_0": "Manussalokakathā niṭṭhitā."
}