{
  "tha-ap94:0.1_0": "Therāpadāna",
  "tha-ap94:0.2_0": "Sudhāvagga",
  "tha-ap94:0.3_0": "2 Sucintikattheraapadāna",
  "tha-ap94:1.1_0": "“Tissassa lokanāthassa,",
  "tha-ap94:1.2_0": "suddhapīṭhamadāsahaṁ;",
  "tha-ap94:1.3_0": "Haṭṭho haṭṭhena cittena,",
  "tha-ap94:1.4_0": "buddhassādiccabandhuno.",
  "tha-ap94:2.1_0": "Aṭṭhārase ito kappe,",
  "tha-ap94:2.2_0": "rājā āsiṁ mahāruci;",
  "tha-ap94:2.3_0": "Bhogo ca vipulo āsi,",
  "tha-ap94:2.4_0": "sayanañca anappakaṁ.",
  "tha-ap94:3.1_0": "Pīṭhaṁ buddhassa datvāna,",
  "tha-ap94:3.2_0": "vippasannena cetasā;",
  "tha-ap94:3.3_0": "Anubhomi sakaṁ kammaṁ,",
  "tha-ap94:3.4_0": "pubbe sukatamattano.",
  "tha-ap94:4.1_0": "Dvenavute ito kappe,",
  "tha-ap94:4.2_0": "yaṁ pīṭhamadadiṁ tadā;",
  "tha-ap94:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap94:4.4_0": "pīṭhadānassidaṁ phalaṁ.",
  "tha-ap94:5.1_0": "Aṭṭhatiṁse ito kappe,",
  "tha-ap94:5.2_0": "tayo te cakkavattino;",
  "tha-ap94:5.3_0": "Ruci uparuci ceva,",
  "tha-ap94:5.4_0": "mahāruci tatiyako.",
  "tha-ap94:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap94:6.2_0": "…pe…",
  "tha-ap94:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap94:7.1_0": "Itthaṁ sudaṁ āyasmā sucintiko thero imā gāthāyo abhāsitthāti.",
  "tha-ap94:8.1_0": "Sucintikattherassāpadānaṁ dutiyaṁ."
}