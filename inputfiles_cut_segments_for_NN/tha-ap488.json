{
  "tha-ap488:0.1_0": "Therāpadāna",
  "tha-ap488:0.2_0": "Paṁsukūlavagga",
  "tha-ap488:0.3_0": "6. Dhātupūjakattheraapadāna",
  "tha-ap488:1.1_0": "“Nibbute lokanāthamhi,",
  "tha-ap488:1.2_0": "siddhatthe lokanāyake;",
  "tha-ap488:1.3_0": "Mama ñātī samānetvā,",
  "tha-ap488:1.4_0": "dhātupūjaṁ akāsahaṁ.",
  "tha-ap488:2.1_0": "Catunnavutito kappe,",
  "tha-ap488:2.2_0": "yaṁ dhātumabhipūjayiṁ;",
  "tha-ap488:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap488:2.4_0": "dhātupūjāyidaṁ phalaṁ.",
  "tha-ap488:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap488:3.2_0": "…pe…",
  "tha-ap488:3.3_0": "viharāmi anāsavo.",
  "tha-ap488:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap488:4.2_0": "…pe…",
  "tha-ap488:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap488:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap488:5.2_0": "…pe…",
  "tha-ap488:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap488:6.1_0": "Itthaṁ sudaṁ āyasmā dhātupūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap488:7.1_0": "Dhātupūjakattherassāpadānaṁ chaṭṭhaṁ."
}