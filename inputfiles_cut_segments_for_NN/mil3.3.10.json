{
  "mil3.3.10:0.1_0": "Milindapañha",
  "mil3.3.10:0.2_0": "Vicāravagga",
  "mil3.3.10:0.3_0": "10. Saññālakkhaṇapañha",
  "mil3.3.10:1.1_0": "“Bhante nāgasena, kiṁlakkhaṇā saññā”ti?",
  "mil3.3.10:1.2_0": "“Sañjānanalakkhaṇā, mahārāja, saññā.",
  "mil3.3.10:1.3_0": "Kiṁ sañjānāti?",
  "mil3.3.10:1.4_0": "Nīlampi sañjānāti,",
  "mil3.3.10:1.4_1": "pītampi sañjānāti,",
  "mil3.3.10:1.4_2": "lohitampi sañjānāti,",
  "mil3.3.10:1.4_3": "odātampi sañjānāti,",
  "mil3.3.10:1.4_4": "mañjiṭṭhampi sañjānāti.",
  "mil3.3.10:1.5_0": "Evaṁ kho, mahārāja, sañjānanalakkhaṇā saññā”ti.",
  "mil3.3.10:2.1_0": "“Opammaṁ karohī”ti.",
  "mil3.3.10:2.2_0": "“Yathā, mahārāja,",
  "mil3.3.10:2.2_1": "rañño bhaṇḍāgāriko bhaṇḍāgāraṁ pavisitvā nīlapītalohitodātamañjiṭṭhāni rājabhogāni rūpāni passitvā sañjānāti.",
  "mil3.3.10:2.3_0": "Evaṁ kho, mahārāja, sañjānanalakkhaṇā saññā”ti.",
  "mil3.3.10:3.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.3.10:4.1_0": "Saññālakkhaṇapañho dasamo."
}