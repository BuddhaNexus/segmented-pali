{
  "ja301:0.1_0": "Jātaka",
  "ja301:0.2_0": "Catukkanipāta",
  "ja301:0.3_0": "Kāliṅgavagga",
  "ja301:0.4_0": "1. Cūḷakāliṅgajātaka",
  "ja301:1.1_0": "“Vivarathimāsaṁ dvāraṁ,",
  "ja301:1.2_0": "Nagaraṁ pavisantu aruṇarājassa;",
  "ja301:1.3_0": "Sīhena susiṭṭhena,",
  "ja301:1.4_0": "Surakkhitaṁ nandisenena”.",
  "ja301:2.1_0": "“Jayo kaliṅgānamasayhasāhinaṁ,",
  "ja301:2.2_0": "Parājayo anayo assakānaṁ;",
  "ja301:2.3_0": "Icceva te bhāsitaṁ brahmacāri,",
  "ja301:2.4_0": "Na ujjubhūtā vitathaṁ bhaṇanti”.",
  "ja301:3.1_0": "“Devā musāvādamupātivattā,",
  "ja301:3.2_0": "Saccaṁ dhanaṁ paramaṁ tesu sakka;",
  "ja301:3.3_0": "Taṁ te musā bhāsitaṁ devarāja,",
  "ja301:3.4_0": "Kiṁ vā paṭicca maghavā mahinda”.",
  "ja301:4.1_0": "“Nanu te sutaṁ brāhmaṇa bhaññamāne,",
  "ja301:4.2_0": "Devā na issanti purisaparakkamassa;",
  "ja301:4.3_0": "Damo samādhi manaso abhejjo,",
  "ja301:4.4_0": "Abyaggatā nikkamanañca kāle;",
  "ja301:4.5_0": "Daḷhañca viriyaṁ purisaparakkamo ca,",
  "ja301:4.6_0": "Teneva āsi vijayo assakānan”ti.",
  "ja301:5.1_0": "Cūḷakāliṅgajātakaṁ paṭhamaṁ."
}