{
  "tika-s0404t12:0_0": "5. Sāmaññavaggo",
  "tika-s0404t12:1_0": "1-10. Sambādhasuttādivaṇṇanā",
  "tika-s0404t12:2_0": "42-51.",
  "tika-s0404t12:2_1": "Pañcamassa paṭhame udāyīti tayo therā udāyī nāma kāḷudāyī, lāḷudāyī,",
  "tika-s0404t12:2_2": "mahāudāyīti,",
  "tika-s0404t12:2_3": "idha kāḷudāyī adhippetoti āha “udāyīti kāḷudāyitthero”ti.",
  "tika-s0404t12:2_4": "Sambādheti sampīḷitataṇhāsaṁkilesādinā sauppīḷanatāya paramasambādhe.",
  "tika-s0404t12:2_5": "Ativiya saṅkaraṭṭhānabhūto hi nīvaraṇasambādho adhippeto.",
  "tika-s0404t12:2_6": "Okāsoti jhānassetaṁ nāmaṁ.",
  "tika-s0404t12:2_7": "Nīvaraṇasambādhābhāvena hi jhānaṁ idha “okāso”ti vuttaṁ.",
  "tika-s0404t12:2_8": "Paṭilīnanisabhoti vā paṭilīno hutvā seṭṭho,",
  "tika-s0404t12:2_9": "paṭilīnānaṁ vā seṭṭhoti paṭilīnanisabho.",
  "tika-s0404t12:2_10": "Paṭilīnā nāma pahīnamānā vuccanti mānussayavasena uṇṇatābhāvato.",
  "tika-s0404t12:2_11": "Yathāha “kathañca,",
  "tika-s0404t12:2_12": "bhikkhave,",
  "tika-s0404t12:2_13": "bhikkhu paṭilīno hoti? Idha,",
  "tika-s0404t12:2_14": "bhikkhave,",
  "tika-s0404t12:2_15": "bhikkhuno asmimāno pahīno hoti ucchinnamūlo tālāvatthukato anabhāvaṁgato āyatiṁ anuppādadhammo”ti.",
  "tika-s0404t12:2_16": "Sesaṁ sabbattha uttānameva.",
  "tika-s0404t12:3_0": "Sambādhasuttādivaṇṇanā niṭṭhitā.",
  "tika-s0404t12:4_0": "Iti manorathapūraṇiyā aṅguttaranikāya-aṭṭhakathāya",
  "tika-s0404t12:5_0": "Navakanipātavaṇṇanāya anuttānatthadīpanā samattā.",
  "tika-s0404t12:6_0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "tika-s0404t12:7_0": "Aṅguttaranikāye",
  "tika-s0404t12:8_0": "Dasakanipāta-ṭīkā",
  "tika-s0404t12:9_0": "1. Paṭhamapaṇṇāsakaṁ"
}