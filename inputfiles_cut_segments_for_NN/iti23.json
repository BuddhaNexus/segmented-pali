{
  "iti23:0.1_0": "Itivuttaka 23",
  "iti23:0.2_0": "Ekakanipāta",
  "iti23:0.3_0": "Tatiyavagga",
  "iti23:0.4_0": "Ubhayatthasutta",
  "iti23:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti23:2.1_0": "“Ekadhammo, bhikkhave,",
  "iti23:2.1_1": "bhāvito bahulīkato ubho atthe samadhigayha tiṭṭhati—",
  "iti23:2.2_0": "diṭṭhadhammikañceva atthaṁ samparāyikañca.",
  "iti23:2.3_0": "Katamo ekadhammo?",
  "iti23:2.4_0": "Appamādo kusalesu dhammesu.",
  "iti23:2.5_0": "Ayaṁ kho, bhikkhave,",
  "iti23:2.5_1": "ekadhammo bhāvito bahulīkato ubho atthe samadhigayha tiṭṭhati—",
  "iti23:2.6_0": "diṭṭhadhammikañceva atthaṁ samparāyikañcā”ti.",
  "iti23:2.7_0": "Etamatthaṁ bhagavā avoca.",
  "iti23:2.8_0": "Tatthetaṁ iti vuccati:",
  "iti23:3.1_0": "“Appamādaṁ pasaṁsanti,",
  "iti23:3.2_0": "puññakiriyāsu paṇḍitā;",
  "iti23:3.3_0": "Appamatto ubho atthe,",
  "iti23:3.4_0": "adhigaṇhāti paṇḍito.",
  "iti23:4.1_0": "Diṭṭhe dhamme ca yo attho,",
  "iti23:4.2_0": "yo cattho samparāyiko;",
  "iti23:4.3_0": "Atthābhisamayā dhīro,",
  "iti23:4.4_0": "paṇḍitoti pavuccatī”ti.",
  "iti23:5.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti23:5.2_0": "Tatiyaṁ."
}