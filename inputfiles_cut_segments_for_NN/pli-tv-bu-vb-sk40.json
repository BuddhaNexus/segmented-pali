{
  "pli-tv-bu-vb-sk40:0.1_0": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk40:0.2_0": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk40:0.3_0": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk40:0.4_0": "Sakkaccavagga",
  "pli-tv-bu-vb-sk40:0.5_0": "40. Ālopasikkhāpada",
  "pli-tv-bu-vb-sk40:1.1_0": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk40:1.2_0": "Tena kho pana samayena chabbaggiyā bhikkhū dīghaṁ ālopaṁ karonti …pe….",
  "pli-tv-bu-vb-sk40:1.3.1_0": "“Parimaṇḍalaṁ ālopaṁ karissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk40:1.4_0": "Parimaṇḍalo ālopo kātabbo.",
  "pli-tv-bu-vb-sk40:1.5_0": "Yo anādariyaṁ paṭicca dīghaṁ ālopaṁ karoti, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk40:1.6.1_0": "Anāpatti—",
  "pli-tv-bu-vb-sk40:1.7_0": "asañcicca,",
  "pli-tv-bu-vb-sk40:1.8_0": "assatiyā,",
  "pli-tv-bu-vb-sk40:1.9_0": "ajānantassa,",
  "pli-tv-bu-vb-sk40:1.10_0": "gilānassa,",
  "pli-tv-bu-vb-sk40:1.11_0": "khajjake,",
  "pli-tv-bu-vb-sk40:1.12_0": "phalāphale,",
  "pli-tv-bu-vb-sk40:1.13_0": "uttaribhaṅge,",
  "pli-tv-bu-vb-sk40:1.14_0": "āpadāsu,",
  "pli-tv-bu-vb-sk40:1.15_0": "ummattakassa,",
  "pli-tv-bu-vb-sk40:1.16_0": "ādikammikassāti.",
  "pli-tv-bu-vb-sk40:1.17_0": "Dasamasikkhāpadaṁ niṭṭhitaṁ.",
  "pli-tv-bu-vb-sk40:1.18_0": "Sakkaccavaggo catuttho."
}