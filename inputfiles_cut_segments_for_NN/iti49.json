{
  "iti49:0.1_0": "Itivuttaka 49",
  "iti49:0.2_0": "Dukanipāta",
  "iti49:0.3_0": "Dutiyavagga",
  "iti49:0.4_0": "Diṭṭhigatasutta",
  "iti49:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti49:2.1_0": "“Dvīhi, bhikkhave,",
  "iti49:2.1_1": "diṭṭhigatehi pariyuṭṭhitā devamanussā olīyanti eke, atidhāvanti eke;",
  "iti49:2.2_0": "cakkhumanto ca passanti.",
  "iti49:3.1_0": "Kathañca, bhikkhave, olīyanti eke?",
  "iti49:3.2_0": "Bhavārāmā, bhikkhave,",
  "iti49:3.2_1": "devamanussā bhavaratā bhavasammuditā tesaṁ bhavanirodhāya dhamme desiyamāne cittaṁ na pakkhandati na pasīdati na santiṭṭhati nādhimuccati.",
  "iti49:3.3_0": "Evaṁ kho, bhikkhave, olīyanti eke.",
  "iti49:4.1_0": "Kathañca, bhikkhave, atidhāvanti eke?",
  "iti49:4.2_0": "Bhaveneva kho paneke aṭṭīyamānā harāyamānā jigucchamānā vibhavaṁ abhinandanti—",
  "iti49:4.3_0": "yato kira, bho,",
  "iti49:4.3_1": "ayaṁ attā kāyassa bhedā paraṁ maraṇā ucchijjati vinassati na hoti paraṁ maraṇā;",
  "iti49:4.4_0": "etaṁ santaṁ etaṁ paṇītaṁ etaṁ yāthāvanti.",
  "iti49:4.5_0": "Evaṁ kho, bhikkhave, atidhāvanti eke.",
  "iti49:5.1_0": "Kathañca, bhikkhave, cakkhumanto passanti?",
  "iti49:5.2_0": "Idha bhikkhu bhūtaṁ bhūtato passati;",
  "iti49:5.3_0": "bhūtaṁ bhūtato disvā bhūtassa nibbidāya virāgāya nirodhāya paṭipanno hoti.",
  "iti49:5.4_0": "Evaṁ kho, bhikkhave, cakkhumanto passantī”ti.",
  "iti49:5.5_0": "Etamatthaṁ bhagavā avoca.",
  "iti49:5.6_0": "Tatthetaṁ iti vuccati:",
  "iti49:6.1_0": "“Ye bhūtaṁ bhūtato disvā,",
  "iti49:6.2_0": "bhūtassa ca atikkamaṁ;",
  "iti49:6.3_0": "Yathābhūte vimuccanti,",
  "iti49:6.4_0": "bhavataṇhā parikkhayā.",
  "iti49:7.1_0": "Sa ve bhūtapariñño so,",
  "iti49:7.2_0": "vītataṇho bhavābhave;",
  "iti49:7.3_0": "Bhūtassa vibhavā bhikkhu,",
  "iti49:7.4_0": "nāgacchati punabbhavan”ti.",
  "iti49:8.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti49:8.2_0": "Dvādasamaṁ.",
  "iti49:8.3_0": "Dutiyo vaggo.",
  "iti49:9.0_0": "Tassuddānaṁ",
  "iti49:9.1_0": "Dve indriyā dve tapanīyā,",
  "iti49:9.2_0": "sīlena apare duve;",
  "iti49:9.3_0": "Anottāpī kuhanā dve ca,",
  "iti49:9.4_0": "saṁvejanīyena te dasa.",
  "iti49:10.1_0": "Vitakkā desanā vijjā,",
  "iti49:10.2_0": "paññā dhammena pañcamaṁ;",
  "iti49:10.3_0": "Ajātaṁ dhātusallānaṁ,",
  "iti49:10.4_0": "sikkhā jāgariyena ca;",
  "iti49:10.5_0": "Apāyadiṭṭhiyā ceva,",
  "iti49:10.6_0": "bāvīsati pakāsitāti.",
  "iti49:10.7_0": "Dukanipāto niṭṭhito."
}