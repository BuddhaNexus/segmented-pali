{
  "atk-s0403a34:0_0": "9. Sītivaggo",
  "atk-s0403a34:1_0": "1. Sītibhāvasuttavaṇṇanā",
  "atk-s0403a34:2_0": "85.",
  "atk-s0403a34:2_1": "Navamassa paṭhame sītibhāvanti sītalabhāvaṁ.",
  "atk-s0403a34:2_2": "Yasmiṁ samaye cittaṁ niggaṇhitabbantiādīsu uddhaccasamaye cittaṁ samādhinā niggahetabbaṁ nāma,",
  "atk-s0403a34:2_3": "kosajjānupatitakāle vīriyena paggahetabbaṁ nāma,",
  "atk-s0403a34:2_4": "nirassādagatakāle samādhinā sampahaṁsitabbaṁ nāma,",
  "atk-s0403a34:2_5": "samappavattakāle bojjhaṅgupekkhāya ajjhupekkhitabbaṁ nāma.",
  "atk-s0403a34:3_0": "2. Āvaraṇasuttavaṇṇanā",
  "atk-s0403a34:4_0": "86.",
  "atk-s0403a34:4_1": "Dutiye kammāvaraṇatāyāti pañcānantariyakammehi.",
  "atk-s0403a34:4_2": "Kilesāvaraṇatāyāti niyatamicchādiṭṭhiyā.",
  "atk-s0403a34:4_3": "Vipākāvaraṇatāyāti akusalavipākapaṭisandhiyā vā kusalavipākehi ahetukapaṭisandhiyā vāti.",
  "atk-s0403a34:5_0": "4-5. Sussūsatisuttādivaṇṇanā",
  "atk-s0403a34:6_0": "88-89.",
  "atk-s0403a34:6_1": "Catutthe anatthanti avaḍḍhiṁ.",
  "atk-s0403a34:6_2": "Atthaṁ riñcatīti vaḍḍhiatthaṁ chaḍḍeti.",
  "atk-s0403a34:6_3": "Ananulomikāyāti sāsanassa ananulomikāya.",
  "atk-s0403a34:6_4": "Pañcame diṭṭhisampadanti sotāpattimaggaṁ.",
  "atk-s0403a34:7_0": "8-11. Abhabbaṭṭhānasuttacatukkavaṇṇanā",
  "atk-s0403a34:8_0": "92-95.",
  "atk-s0403a34:8_1": "Aṭṭhame anāgamanīyaṁ vatthunti anupagantabbaṁ kāraṇaṁ,",
  "atk-s0403a34:8_2": "pañcannaṁ verānaṁ dvāsaṭṭhiyā ca diṭṭhigatānametaṁ adhivacanaṁ.",
  "atk-s0403a34:8_3": "Aṭṭhamaṁ bhavanti kāmāvacare aṭṭhamaṁ paṭisandhiṁ.",
  "atk-s0403a34:8_4": "Navame kotūhalamaṅgalenāti diṭṭhasutamutamaṅgalena.",
  "atk-s0403a34:8_5": "Dasame sayaṁkatantiādīni attadiṭṭhivasena vuttāni.",
  "atk-s0403a34:8_6": "Adhiccasamuppannanti ahetunibbattaṁ.",
  "atk-s0403a34:8_7": "Sesaṁ sabbattha uttānamevāti.",
  "atk-s0403a34:9_0": "Sītivaggo navamo."
}