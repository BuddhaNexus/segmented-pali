{
  "kv7.5:0.1_0": "Kathāvatthu",
  "kv7.5:0.2_0": "Mahāpaṇṇāsaka",
  "kv7.5:0.3_0": "Sattamavagga",
  "kv7.5:0.4_0": "Paribhogamayapuññakathā",
  "kv7.5:1.1_0": "Paribhogamayaṁ puññaṁ vaḍḍhatīti?",
  "kv7.5:1.2_0": "Āmantā.",
  "kv7.5:1.3_0": "Paribhogamayo phasso vaḍḍhati, vedanā vaḍḍhati, saññā vaḍḍhati, cetanā vaḍḍhati, cittaṁ vaḍḍhati, saddhā vaḍḍhati, vīriyaṁ vaḍḍhati, sati vaḍḍhati, samādhi vaḍḍhati, paññā vaḍḍhatīti?",
  "kv7.5:1.4_0": "Na hevaṁ vattabbe …pe….",
  "kv7.5:2.1_0": "Paribhogamayaṁ puññaṁ vaḍḍhatīti?",
  "kv7.5:2.2_0": "Āmantā.",
  "kv7.5:2.3_0": "Latā viya vaḍḍhati, māluvā viya vaḍḍhati, rukkho viya vaḍḍhati, tiṇaṁ viya vaḍḍhati, muñjapuñjo viya vaḍḍhatīti?",
  "kv7.5:2.4_0": "Na hevaṁ vattabbe …pe….",
  "kv7.5:3.1_0": "Paribhogamayaṁ puññaṁ vaḍḍhatīti?",
  "kv7.5:3.2_0": "Āmantā.",
  "kv7.5:3.3_0": "Dāyako dānaṁ datvā na samannāharati, hoti puññanti?",
  "kv7.5:3.4_0": "Āmantā.",
  "kv7.5:3.5_0": "Anāvaṭṭentassa hoti …",
  "kv7.5:3.6_0": "anābhogassa hoti …",
  "kv7.5:3.7_0": "asamannāharantassa hoti …",
  "kv7.5:3.8_0": "amanasikarontassa hoti …",
  "kv7.5:3.9_0": "acetayantassa hoti …",
  "kv7.5:3.10_0": "apatthayantassa hoti … appaṇidahantassa hotīti?",
  "kv7.5:3.11_0": "Na hevaṁ vattabbe …pe…",
  "kv7.5:3.12_0": "nanu āvaṭṭentassa hoti …",
  "kv7.5:3.13_0": "ābhogassa hoti …",
  "kv7.5:3.14_0": "samannāharantassa hoti …",
  "kv7.5:3.15_0": "manasikarontassa hoti …",
  "kv7.5:3.16_0": "cetayantassa hoti …",
  "kv7.5:3.17_0": "patthayantassa hoti … paṇidahantassa hotīti?",
  "kv7.5:3.18_0": "Āmantā.",
  "kv7.5:3.19_0": "Hañci āvaṭṭentassa hoti …",
  "kv7.5:3.20_0": "ābhogassa hoti …",
  "kv7.5:3.21_0": "samannāharantassa hoti …",
  "kv7.5:3.22_0": "manasikarontassa hoti …",
  "kv7.5:3.23_0": "cetayantassa hoti …",
  "kv7.5:3.24_0": "patthayantassa hoti … paṇidahantassa hoti, no ca vata re vattabbe—",
  "kv7.5:3.25_0": "“paribhogamayaṁ puññaṁ vaḍḍhatī”ti.",
  "kv7.5:4.1_0": "Paribhogamayaṁ puññaṁ vaḍḍhatīti?",
  "kv7.5:4.2_0": "Āmantā.",
  "kv7.5:4.3_0": "Dāyako dānaṁ datvā kāmavitakkaṁ vitakketi, byāpādavitakkaṁ vitakketi, vihiṁsāvitakkaṁ vitakketi, hoti puññanti?",
  "kv7.5:4.4_0": "Āmantā.",
  "kv7.5:4.5_0": "Dvinnaṁ phassānaṁ …pe…",
  "kv7.5:4.6_0": "dvinnaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv7.5:4.7_0": "Na hevaṁ vattabbe …pe…",
  "kv7.5:4.8_0": "dvinnaṁ phassānaṁ …pe…",
  "kv7.5:4.9_0": "dvinnaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv7.5:4.10_0": "Āmantā.",
  "kv7.5:4.11_0": "Kusalākusalā sāvajjānavajjā hīnapaṇītā kaṇhasukkasappaṭibhāgā dhammā sammukhībhāvaṁ āgacchantīti?",
  "kv7.5:4.12_0": "Na hevaṁ vattabbe …pe…",
  "kv7.5:4.13_0": "kusalākusalā sāvajjānavajjā hīnapaṇītā kaṇhasukkasappaṭibhāgā dhammā sammukhībhāvaṁ āgacchantīti?",
  "kv7.5:4.14_0": "Āmantā.",
  "kv7.5:4.15_0": "Nanu vuttaṁ bhagavatā—",
  "kv7.5:4.16_0": "“cattārimāni, bhikkhave, suvidūravidūrāni.",
  "kv7.5:4.17_0": "Katamāni cattāri?",
  "kv7.5:4.18_0": "Nabhañca, bhikkhave, pathavī ca—",
  "kv7.5:4.19_0": "idaṁ paṭhamaṁ suvidūravidūraṁ.",
  "kv7.5:4.20_0": "Orimañca, bhikkhave,",
  "kv7.5:4.20_1": "tīraṁ samuddassa pārimañca tīraṁ—",
  "kv7.5:4.21_0": "idaṁ dutiyaṁ suvidūravidūraṁ.",
  "kv7.5:4.22_0": "Yato ca, bhikkhave,",
  "kv7.5:4.22_1": "verocano abbhudeti yattha ca atthameti—",
  "kv7.5:4.23_0": "idaṁ tatiyaṁ suvidūravidūraṁ.",
  "kv7.5:4.24_0": "Satañca, bhikkhave, dhammo asatañca dhammo—",
  "kv7.5:4.25_0": "idaṁ catutthaṁ suvidūravidūraṁ.",
  "kv7.5:4.26_0": "Imāni kho, bhikkhave, cattāri suvidūravidūrānīti.",
  "kv7.5:5.1_0": "Nabhañca dūre pathavī ca dūre,",
  "kv7.5:5.2_0": "Pāraṁ samuddassa tadāhu dūre;",
  "kv7.5:5.3_0": "Yato ca verocano abbhudeti,",
  "kv7.5:5.4_0": "Pabhaṅkaro yattha ca atthameti;",
  "kv7.5:5.5_0": "Tato have dūrataraṁ vadanti,",
  "kv7.5:5.6_0": "Satañca dhammaṁ asatañca dhammaṁ.",
  "kv7.5:6.1_0": "Abyāyiko hoti sataṁ samāgamo,",
  "kv7.5:6.2_0": "Yāvampi tiṭṭheyya tatheva hoti;",
  "kv7.5:6.3_0": "Khippañhi veti asataṁ samāgamo,",
  "kv7.5:6.4_0": "Tasmā sataṁ dhammo asabbhi ārakā”ti.",
  "kv7.5:7.1_0": "Attheva suttantoti?",
  "kv7.5:7.2_0": "Āmantā.",
  "kv7.5:7.3_0": "Tena hi na vattabbaṁ—",
  "kv7.5:7.4_0": "“kusalākusalā sāvajjānavajjā hīnapaṇītā kaṇhasukkasappaṭibhāgā dhammā sammukhībhāvaṁ āgacchantī”ti.",
  "kv7.5:8.1_0": "Na vattabbaṁ—",
  "kv7.5:8.2_0": "“paribhogamayaṁ puññaṁ vaḍḍhatī”ti?",
  "kv7.5:8.3_0": "Āmantā.",
  "kv7.5:8.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv7.5:9.1_0": "“Ārāmaropā vanaropā,",
  "kv7.5:9.2_0": "ye janā setukārakā;",
  "kv7.5:9.3_0": "Papañca udapānañca,",
  "kv7.5:9.4_0": "ye dadanti upassayaṁ.",
  "kv7.5:10.1_0": "Tesaṁ divā ca ratto ca,",
  "kv7.5:10.2_0": "sadā puññaṁ pavaḍḍhati;",
  "kv7.5:10.3_0": "Dhammaṭṭhā sīlasampannā,",
  "kv7.5:10.4_0": "te janā saggagāmino”ti.",
  "kv7.5:11.1_0": "Attheva suttantoti?",
  "kv7.5:11.2_0": "Āmantā.",
  "kv7.5:11.3_0": "Tena hi paribhogamayaṁ puññaṁ vaḍḍhatīti.",
  "kv7.5:12.1_0": "Na vattabbaṁ—",
  "kv7.5:12.2_0": "“paribhogamayaṁ puññaṁ vaḍḍhatī”ti?",
  "kv7.5:12.3_0": "Āmantā.",
  "kv7.5:12.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv7.5:12.5_0": "“cattārome, bhikkhave,",
  "kv7.5:12.5_1": "puññābhisandā kusalābhisandā sukhassāhārā sovaggikā sukhavipākā saggasaṁvattanikā iṭṭhāya kantāya manāpāya hitāya sukhāya saṁvattanti.",
  "kv7.5:12.6_0": "Katame cattāro?",
  "kv7.5:12.7_0": "Yassa, bhikkhave,",
  "kv7.5:12.7_1": "bhikkhu cīvaraṁ paribhuñjamāno appamāṇaṁ cetosamādhiṁ upasampajja viharati,",
  "kv7.5:12.7_2": "appamāṇo tassa puññābhisando kusalābhisando sukhassāhāro sovaggiko sukhavipāko saggasaṁvattaniko iṭṭhāya kantāya manāpāya hitāya sukhāya saṁvattati.",
  "kv7.5:12.8_0": "Yassa, bhikkhave,",
  "kv7.5:12.8_1": "bhikkhu piṇḍapātaṁ paribhuñjamāno …pe…",
  "kv7.5:12.9_0": "senāsanaṁ paribhuñjamāno …pe…",
  "kv7.5:12.10_0": "gilānapaccayabhesajjaparikkhāraṁ paribhuñjamāno appamāṇaṁ cetosamādhiṁ upasampajja viharati,",
  "kv7.5:12.10_1": "appamāṇo tassa puññābhisando kusalābhisando sukhassāhāro sovaggiko sukhavipāko saggasaṁvattaniko iṭṭhāya kantāya manāpāya hitāya sukhāya saṁvattati.",
  "kv7.5:12.11_0": "Ime kho, bhikkhave,",
  "kv7.5:12.11_1": "cattāro puññābhisandā kusalābhisandā sukhassāhārā sovaggikā sukhavipākā saggasaṁvattanikā iṭṭhāya kantāya manāpāya hitāya sukhāya saṁvattantī”ti.",
  "kv7.5:12.12_0": "Attheva suttantoti?",
  "kv7.5:12.13_0": "Āmantā.",
  "kv7.5:12.14_0": "Tena hi paribhogamayaṁ puññaṁ vaḍḍhatīti.",
  "kv7.5:13.1_0": "Paribhogamayaṁ puññaṁ vaḍḍhatīti?",
  "kv7.5:13.2_0": "Āmantā.",
  "kv7.5:13.3_0": "Dāyako dānaṁ deti,",
  "kv7.5:13.3_1": "paṭiggāhako paṭiggahetvā na paribhuñjati chaḍḍeti vissajjeti, hoti puññanti?",
  "kv7.5:13.4_0": "Āmantā.",
  "kv7.5:13.5_0": "Hañci dāyako dānaṁ deti,",
  "kv7.5:13.5_1": "paṭiggāhako paṭiggahetvā na paribhuñjati chaḍḍeti vissajjeti, hoti puññaṁ;",
  "kv7.5:13.6_0": "no ca vata re vattabbe—",
  "kv7.5:13.7_0": "“paribhogamayaṁ puññaṁ vaḍḍhatī”ti.",
  "kv7.5:14.1_0": "Paribhogamayaṁ puññaṁ vaḍḍhatīti?",
  "kv7.5:14.2_0": "Āmantā.",
  "kv7.5:14.3_0": "Dāyako dānaṁ deti,",
  "kv7.5:14.3_1": "paṭiggāhake paṭiggahite rājāno vā haranti, corā vā haranti, aggi vā dahati, udakaṁ vā vahati, appiyā vā dāyādā haranti, hoti puññanti?",
  "kv7.5:14.4_0": "Āmantā.",
  "kv7.5:14.5_0": "Hañci dāyako dānaṁ deti,",
  "kv7.5:14.5_1": "paṭiggāhake paṭiggahite rājāno vā haranti, corā vā haranti, aggi vā dahati, udakaṁ vā vahati, appiyā vā dāyādā haranti, hoti puññaṁ;",
  "kv7.5:14.6_0": "no ca vata re vattabbe—",
  "kv7.5:14.7_0": "“paribhogamayaṁ puññaṁ vaḍḍhatī”ti.",
  "kv7.5:14.8_0": "Paribhogamayapuññakathā niṭṭhitā."
}