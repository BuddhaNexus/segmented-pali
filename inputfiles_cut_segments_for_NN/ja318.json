{
  "ja318:0.1_0": "Jātaka",
  "ja318:0.2_0": "Catukkanipāta",
  "ja318:0.3_0": "Pucimandavagga",
  "ja318:0.4_0": "8. Kaṇaverajātaka",
  "ja318:1.1_0": "“Yaṁ taṁ vasantasamaye,",
  "ja318:1.2_0": "kaṇaveresu bhāṇusu;",
  "ja318:1.3_0": "Sāmaṁ bāhāya pīḷesi,",
  "ja318:1.4_0": "sā taṁ ārogyamabravi”.",
  "ja318:2.1_0": "“Ambho na kira saddheyyaṁ,",
  "ja318:2.2_0": "yaṁ vāto pabbataṁ vahe;",
  "ja318:2.3_0": "Pabbatañce vahe vāto,",
  "ja318:2.4_0": "sabbampi pathaviṁ vahe;",
  "ja318:2.5_0": "Yattha sāmā kālakatā,",
  "ja318:2.6_0": "sā maṁ ārogyamabravi”.",
  "ja318:3.1_0": "“Na ceva sā kālakatā,",
  "ja318:3.2_0": "na ca sā aññamicchati;",
  "ja318:3.3_0": "Ekabhattikinī sāmā,",
  "ja318:3.4_0": "tameva abhikaṅkhati”.",
  "ja318:4.1_0": "“Asanthutaṁ maṁ cirasanthutena,",
  "ja318:4.2_0": "Nimīni sāmā adhuvaṁ dhuvena;",
  "ja318:4.3_0": "Mayāpi sāmā nimineyya aññaṁ,",
  "ja318:4.4_0": "Ito ahaṁ dūrataraṁ gamissan”ti.",
  "ja318:5.1_0": "Kaṇaverajātakaṁ aṭṭhamaṁ."
}