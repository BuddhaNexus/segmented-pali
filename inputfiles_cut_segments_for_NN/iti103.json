{
  "iti103:0.1_0": "Itivuttaka 103",
  "iti103:0.2_0": "Catukkanipāta",
  "iti103:0.3_0": "Brāhmaṇadhammayāgavagga",
  "iti103:0.4_0": "Samaṇabrāhmaṇasutta",
  "iti103:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti103:2.1_0": "“Ye hi keci, bhikkhave,",
  "iti103:2.1_1": "samaṇā vā brāhmaṇā vā ‘idaṁ dukkhan’ti yathābhūtaṁ nappajānanti;",
  "iti103:2.1_2": "‘ayaṁ dukkhasamudayo’ti yathābhūtaṁ nappajānanti;",
  "iti103:2.1_3": "‘ayaṁ dukkhanirodho’ti yathābhūtaṁ nappajānanti;",
  "iti103:2.1_4": "‘ayaṁ dukkhanirodhagāminī paṭipadā’ti yathābhūtaṁ nappajānanti—",
  "iti103:2.2_0": "na me te, bhikkhave,",
  "iti103:2.2_1": "samaṇā vā brāhmaṇā vā samaṇesu vā samaṇasammatā brāhmaṇesu vā brāhmaṇasammatā,",
  "iti103:2.2_2": "na ca panete āyasmanto sāmaññatthaṁ vā brahmaññatthaṁ vā diṭṭheva dhamme sayaṁ abhiññā sacchikatvā upasampajja viharanti.",
  "iti103:3.1_0": "Ye ca kho keci, bhikkhave,",
  "iti103:3.1_1": "samaṇā vā brāhmaṇā vā ‘idaṁ dukkhan’ti yathābhūtaṁ pajānanti;",
  "iti103:3.1_2": "‘ayaṁ dukkhasamudayo’ti yathābhūtaṁ pajānanti;",
  "iti103:3.1_3": "‘ayaṁ dukkhanirodho’ti yathābhūtaṁ pajānanti;",
  "iti103:3.1_4": "‘ayaṁ dukkhanirodhagāminī paṭipadā’ti yathābhūtaṁ pajānanti—",
  "iti103:3.2_0": "te kho me, bhikkhave,",
  "iti103:3.2_1": "samaṇā vā brāhmaṇā vā samaṇesu ceva samaṇasammatā brāhmaṇesu ca brāhmaṇasammatā,",
  "iti103:3.2_2": "te ca panāyasmanto sāmaññatthañca brahmaññatthañca diṭṭheva dhamme sayaṁ abhiññā sacchikatvā upasampajja viharantī”ti.",
  "iti103:3.3_0": "Etamatthaṁ bhagavā avoca.",
  "iti103:3.4_0": "Tatthetaṁ iti vuccati:",
  "iti103:4.1_0": "“Ye dukkhaṁ nappajānanti,",
  "iti103:4.2_0": "atho dukkhassa sambhavaṁ;",
  "iti103:4.3_0": "Yattha ca sabbaso dukkhaṁ,",
  "iti103:4.4_0": "asesaṁ uparujjhati;",
  "iti103:4.5_0": "Tañca maggaṁ na jānanti,",
  "iti103:4.6_0": "dukkhūpasamagāminaṁ.",
  "iti103:5.1_0": "Cetovimuttihīnā te,",
  "iti103:5.2_0": "Atho paññāvimuttiyā;",
  "iti103:5.3_0": "Abhabbā te antakiriyāya,",
  "iti103:5.4_0": "Te ve jātijarūpagā.",
  "iti103:6.1_0": "Ye ca dukkhaṁ pajānanti,",
  "iti103:6.2_0": "atho dukkhassa sambhavaṁ;",
  "iti103:6.3_0": "Yattha ca sabbaso dukkhaṁ,",
  "iti103:6.4_0": "asesaṁ uparujjhati;",
  "iti103:6.5_0": "Tañca maggaṁ pajānanti,",
  "iti103:6.6_0": "dukkhūpasamagāminaṁ.",
  "iti103:7.1_0": "Cetovimuttisampannā,",
  "iti103:7.2_0": "atho paññāvimuttiyā;",
  "iti103:7.3_0": "Bhabbā te antakiriyāya,",
  "iti103:7.4_0": "na te jātijarūpagā”ti.",
  "iti103:8.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti103:8.2_0": "Catutthaṁ."
}