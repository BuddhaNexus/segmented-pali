{
  "ja152:0.1_0": "Jātaka",
  "ja152:0.2_0": "Dukanipāta",
  "ja152:0.3_0": "Daḷhavagga",
  "ja152:0.4_0": "2. Siṅgālajātaka",
  "ja152:1.1_0": "“Asamekkhitakammantaṁ,",
  "ja152:1.2_0": "turitābhinipātinaṁ;",
  "ja152:1.3_0": "Sāni kammāni tappenti,",
  "ja152:1.4_0": "uṇhaṁvajjhohitaṁ mukhe”.",
  "ja152:2.1_0": "“Sīho ca sīhanādena,",
  "ja152:2.2_0": "daddaraṁ abhinādayi;",
  "ja152:2.3_0": "Sutvā sīhassa nigghosaṁ,",
  "ja152:2.4_0": "siṅgālo daddare vasaṁ;",
  "ja152:2.5_0": "Bhīto santāsamāpādi,",
  "ja152:2.6_0": "hadayañcassa apphalī”ti.",
  "ja152:3.1_0": "Siṅgālajātakaṁ dutiyaṁ."
}