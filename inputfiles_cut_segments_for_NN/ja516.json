{
  "ja516:0.1_0": "Jātaka",
  "ja516:0.2_0": "Tiṁsanipāta",
  "ja516:0.3_0": "Kiṁchandavagga",
  "ja516:0.4_0": "6. Mahākapijātaka",
  "ja516:1.1_0": "Bārāṇasyaṁ ahū rājā,",
  "ja516:1.2_0": "kāsīnaṁ raṭṭhavaḍḍhano;",
  "ja516:1.3_0": "Mittāmaccaparibyūḷho,",
  "ja516:1.4_0": "agamāsi migājinaṁ.",
  "ja516:2.1_0": "Tattha brāhmaṇamaddakkhi,",
  "ja516:2.2_0": "setaṁ citraṁ kilāsinaṁ;",
  "ja516:2.3_0": "Viddhastaṁ koviḷāraṁva,",
  "ja516:2.4_0": "kisaṁ dhamanisanthataṁ.",
  "ja516:3.1_0": "Paramakāruññataṁ pattaṁ,",
  "ja516:3.2_0": "disvā kicchagataṁ naraṁ;",
  "ja516:3.3_0": "Avaca byamhito rājā,",
  "ja516:3.4_0": "“yakkhānaṁ katamo nusi.",
  "ja516:4.1_0": "Hatthapādā ca te setā,",
  "ja516:4.2_0": "tato setataraṁ siro;",
  "ja516:4.3_0": "Gattaṁ kammāsavaṇṇaṁ te,",
  "ja516:4.4_0": "kilāsabahulo casi.",
  "ja516:5.1_0": "Vaṭṭanāvaḷi saṅkāsā,",
  "ja516:5.2_0": "piṭṭhi te ninnatunnatā;",
  "ja516:5.3_0": "Kāḷapabbāva te aṅgā,",
  "ja516:5.4_0": "nāññaṁ passāmi edisaṁ.",
  "ja516:6.1_0": "Ugghaṭṭapādo tasito,",
  "ja516:6.2_0": "kiso dhamanisanthato;",
  "ja516:6.3_0": "Chāto ātattarūposi,",
  "ja516:6.4_0": "kutosi kattha gacchasi.",
  "ja516:7.1_0": "Duddasī appakārosi,",
  "ja516:7.2_0": "dubbaṇṇo bhīmadassano;",
  "ja516:7.3_0": "Janetti yāpi te mātā,",
  "ja516:7.4_0": "na taṁ iccheyya passituṁ.",
  "ja516:8.1_0": "Kiṁ kammamakaraṁ pubbe,",
  "ja516:8.2_0": "Kaṁ avajjhaṁ aghātayi;",
  "ja516:8.3_0": "Kibbisaṁ yaṁ karitvāna,",
  "ja516:8.4_0": "Idaṁ dukkhaṁ upāgami”.",
  "ja516:9.1_0": "“Taggha te ahamakkhissaṁ,",
  "ja516:9.2_0": "yathāpi kusalo tathā;",
  "ja516:9.3_0": "Saccavādiñhi lokasmiṁ,",
  "ja516:9.4_0": "pasaṁsantīdha paṇḍitā.",
  "ja516:10.1_0": "Eko caraṁ gogaveso,",
  "ja516:10.2_0": "mūḷho accasariṁ vane;",
  "ja516:10.3_0": "Araññe irīṇe vivane,",
  "ja516:10.4_0": "nānākuñjarasevite.",
  "ja516:11.1_0": "Vāḷamigānucarite,",
  "ja516:11.2_0": "vippanaṭṭhosmi kānane;",
  "ja516:11.3_0": "Acariṁ tattha sattāhaṁ,",
  "ja516:11.4_0": "khuppipāsasamappito.",
  "ja516:12.1_0": "Tattha tindukamaddakkhiṁ,",
  "ja516:12.2_0": "visamaṭṭhaṁ bubhukkhito;",
  "ja516:12.3_0": "Papātamabhilambantaṁ,",
  "ja516:12.4_0": "sampannaphaladhārinaṁ.",
  "ja516:13.1_0": "Vātassitāni bhakkhesiṁ,",
  "ja516:13.2_0": "tāni rucciṁsu me bhusaṁ;",
  "ja516:13.3_0": "Atitto rukkhamārūhiṁ,",
  "ja516:13.4_0": "tattha hessāmi āsito.",
  "ja516:14.1_0": "Ekaṁ me bhakkhitaṁ āsi,",
  "ja516:14.2_0": "dutiyaṁ abhipatthitaṁ;",
  "ja516:14.3_0": "Tato sā bhañjatha sākhā,",
  "ja516:14.4_0": "chinnā pharasunā viya.",
  "ja516:15.1_0": "Sohaṁ sahāva sākhāhi,",
  "ja516:15.2_0": "uddhampādo avaṁsiro;",
  "ja516:15.3_0": "Appatiṭṭhe anālambe,",
  "ja516:15.4_0": "giriduggasmi pāpataṁ.",
  "ja516:16.1_0": "Yasmā ca vāri gambhīraṁ,",
  "ja516:16.2_0": "tasmā na samapajjisaṁ;",
  "ja516:16.3_0": "Tattha sesiṁ nirānando,",
  "ja516:16.4_0": "anūnā dasa rattiyo.",
  "ja516:17.1_0": "Athettha kapi māgañchi,",
  "ja516:17.2_0": "gonaṅgulo darīcaro;",
  "ja516:17.3_0": "Sākhāhi sākhaṁ vicaranto,",
  "ja516:17.4_0": "khādamāno dumapphalaṁ.",
  "ja516:18.1_0": "So maṁ disvā kisaṁ paṇḍuṁ,",
  "ja516:18.2_0": "kāruññamakaraṁ mayi;",
  "ja516:18.3_0": "Ambho ko nāma so ettha,",
  "ja516:18.4_0": "evaṁ dukkhena aṭṭito.",
  "ja516:19.1_0": "Manusso amanusso vā,",
  "ja516:19.2_0": "attānaṁ me pavedaya;",
  "ja516:19.3_0": "Tassañjaliṁ paṇāmetvā,",
  "ja516:19.4_0": "idaṁ vacanamabraviṁ.",
  "ja516:20.1_0": "Manussohaṁ byasampatto,",
  "ja516:20.2_0": "sā me natthi ito gati;",
  "ja516:20.3_0": "Taṁ vo vadāmi bhaddaṁ vo,",
  "ja516:20.4_0": "tvañca me saraṇaṁ bhava.",
  "ja516:21.1_0": "Garuṁ silaṁ gahetvāna,",
  "ja516:21.2_0": "vicarī pabbate kapi;",
  "ja516:21.3_0": "Silāya yoggaṁ katvāna,",
  "ja516:21.4_0": "nisabho etadabravi.",
  "ja516:22.1_0": "Ehi me piṭṭhimāruyha,",
  "ja516:22.2_0": "gīvaṁ gaṇhāhi bāhubhi;",
  "ja516:22.3_0": "Ahaṁ taṁ uddharissāmi,",
  "ja516:22.4_0": "giriduggata vegasā.",
  "ja516:23.1_0": "Tassa taṁ vacanaṁ sutvā,",
  "ja516:23.2_0": "vānarindassa sirīmato;",
  "ja516:23.3_0": "Piṭṭhimāruyha dhīrassa,",
  "ja516:23.4_0": "gīvaṁ bāhāhi aggahiṁ.",
  "ja516:24.1_0": "So maṁ tato samuṭṭhāsi,",
  "ja516:24.2_0": "tejassī balavā kapi;",
  "ja516:24.3_0": "Vihaññamāno kicchena,",
  "ja516:24.4_0": "giriduggata vegasā.",
  "ja516:25.1_0": "Uddharitvāna maṁ santo,",
  "ja516:25.2_0": "nisabho etadabravi;",
  "ja516:25.3_0": "Iṅgha maṁ samma rakkhassu,",
  "ja516:25.4_0": "pasupissaṁ muhuttakaṁ.",
  "ja516:26.1_0": "Sīhā byagghā ca dīpī ca,",
  "ja516:26.2_0": "acchakokataracchayo;",
  "ja516:26.3_0": "Te maṁ pamattaṁ hiṁseyyuṁ,",
  "ja516:26.4_0": "te tvaṁ disvā nivāraya.",
  "ja516:27.1_0": "Evaṁ me parittātūna,",
  "ja516:27.2_0": "pasupī so muhuttakaṁ;",
  "ja516:27.3_0": "Tadāhaṁ pāpikaṁ diṭṭhiṁ,",
  "ja516:27.4_0": "paṭilacchiṁ ayoniso.",
  "ja516:28.1_0": "Bhakkho ayaṁ manussānaṁ,",
  "ja516:28.2_0": "yathā caññe vane migā;",
  "ja516:28.3_0": "Yaṁ nūnimaṁ vadhitvāna,",
  "ja516:28.4_0": "chāto khādeyya vānaraṁ.",
  "ja516:29.1_0": "Asito ca gamissāmi,",
  "ja516:29.2_0": "maṁsamādāya sambalaṁ;",
  "ja516:29.3_0": "Kantāraṁ nittharissāmi,",
  "ja516:29.4_0": "pātheyyaṁ me bhavissati.",
  "ja516:30.1_0": "Tato silaṁ gahetvāna,",
  "ja516:30.2_0": "matthakaṁ sannitāḷayiṁ;",
  "ja516:30.3_0": "Mama gattakilantassa,",
  "ja516:30.4_0": "pahāro dubbalo ahu.",
  "ja516:31.1_0": "So ca vegenudappatto,",
  "ja516:31.2_0": "kapi ruhira makkhito;",
  "ja516:31.3_0": "Assupuṇṇehi nettehi,",
  "ja516:31.4_0": "rodanto maṁ udikkhati.",
  "ja516:32.1_0": "‘Māyyomaṁ kari bhaddante,",
  "ja516:32.2_0": "tvañca nāmedisaṁ kari;",
  "ja516:32.3_0": "Tvañca kho nāma dīghāvu,",
  "ja516:32.4_0": "aññe vāretumarahasi.",
  "ja516:33.1_0": "Aho vata re purisa,",
  "ja516:33.2_0": "tāvadukkarakāraka;",
  "ja516:33.3_0": "Edisā visamā duggā,",
  "ja516:33.4_0": "papātā uddhato mayā.",
  "ja516:34.1_0": "Ānīto paralokāva,",
  "ja516:34.2_0": "dubbheyyaṁ maṁ amaññatha;",
  "ja516:34.3_0": "Taṁ tena pāpadhammena,",
  "ja516:34.4_0": "pāpaṁ pāpena cintitaṁ.",
  "ja516:35.1_0": "Mā heva tvaṁ adhammaṭṭha,",
  "ja516:35.2_0": "vedanaṁ kaṭukaṁ phusi;",
  "ja516:35.3_0": "Mā heva pāpakammaṁ taṁ,",
  "ja516:35.4_0": "phalaṁ veḷuṁva taṁ vadhi.",
  "ja516:36.1_0": "Tayime natthi vissāso,",
  "ja516:36.2_0": "pāpadhamma asaññata;",
  "ja516:36.3_0": "Ehi me piṭṭhito gaccha,",
  "ja516:36.4_0": "dissamānova santike.",
  "ja516:37.1_0": "Muttosi hatthā vāḷānaṁ,",
  "ja516:37.2_0": "pattosi mānusiṁ padaṁ;",
  "ja516:37.3_0": "Esa maggo adhammaṭṭha,",
  "ja516:37.4_0": "tena gaccha yathāsukhaṁ’.",
  "ja516:38.1_0": "Idaṁ vatvā giricaro,",
  "ja516:38.2_0": "rahade pakkhalya matthakaṁ;",
  "ja516:38.3_0": "Assūni sampamajjitvā,",
  "ja516:38.4_0": "tato pabbatamāruhi.",
  "ja516:39.1_0": "Sohaṁ tenābhisattosmi,",
  "ja516:39.2_0": "pariḷāhena aṭṭito;",
  "ja516:39.3_0": "Ḍayhamānena gattena,",
  "ja516:39.4_0": "vāriṁ pātuṁ upāgamiṁ.",
  "ja516:40.1_0": "Agginā viya santatto,",
  "ja516:40.2_0": "rahado ruhiramakkhito;",
  "ja516:40.3_0": "Pubbalohitasaṅkāso,",
  "ja516:40.4_0": "sabbo me samapajjatha.",
  "ja516:41.1_0": "Yāvanto udabindūni,",
  "ja516:41.2_0": "kāyasmiṁ nipatiṁsu me;",
  "ja516:41.3_0": "Tāvanto gaṇḍa jāyetha,",
  "ja516:41.4_0": "addhabeluvasādisā.",
  "ja516:42.1_0": "Pabhinnā pagghariṁsu me,",
  "ja516:42.2_0": "kuṇapā pubbalohitā;",
  "ja516:42.3_0": "Yena yeneva gacchāmi,",
  "ja516:42.4_0": "gāmesu nigamesu ca.",
  "ja516:43.1_0": "Daṇḍahatthā nivārenti,",
  "ja516:43.2_0": "itthiyo purisā ca maṁ;",
  "ja516:43.3_0": "Okkitā pūtigandhena,",
  "ja516:43.4_0": "māssu orena āgamā.",
  "ja516:44.1_0": "Etādisaṁ idaṁ dukkhaṁ,",
  "ja516:44.2_0": "satta vassāni dāni me;",
  "ja516:44.3_0": "Anubhomi sakaṁ kammaṁ,",
  "ja516:44.4_0": "pubbe dukkaṭamattano.",
  "ja516:45.1_0": "Taṁ vo vadāmi bhaddante,",
  "ja516:45.2_0": "yāvantettha samāgatā;",
  "ja516:45.3_0": "Māssu mittāna dubbhittho,",
  "ja516:45.4_0": "mittadubbho hi pāpako.",
  "ja516:46.1_0": "Kuṭṭhī kilāsī bhavati,",
  "ja516:46.2_0": "yo mittānidha dubbhati;",
  "ja516:46.3_0": "Kāyassa bhedā mittaddu,",
  "ja516:46.4_0": "nirayaṁ sopapajjatī”ti.",
  "ja516:47.1_0": "Mahākapijātakaṁ chaṭṭhaṁ."
}