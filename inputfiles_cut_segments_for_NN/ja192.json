{
  "ja192:0.1_0": "Jātaka",
  "ja192:0.2_0": "Dukanipāta",
  "ja192:0.3_0": "Ruhakavagga",
  "ja192:0.4_0": "2. Sirikāḷakaṇṇijātaka",
  "ja192:1.1_0": "“Itthī siyā rūpavatī,",
  "ja192:1.2_0": "sā ca sīlavatī siyā;",
  "ja192:1.3_0": "Puriso taṁ na iccheyya,",
  "ja192:1.4_0": "saddahāsi mahosadha”.",
  "ja192:2.1_0": "“Saddahāmi mahārāja,",
  "ja192:2.2_0": "puriso dubbhago siyā;",
  "ja192:2.3_0": "Sirī ca kāḷakaṇṇī ca,",
  "ja192:2.4_0": "na samenti kudācanan”ti.",
  "ja192:3.1_0": "Sirikāḷakaṇṇijātakaṁ dutiyaṁ."
}