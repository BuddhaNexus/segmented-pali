{
  "tika-s0401t18:0_0": "20. Amatavaggavaṇṇanā",
  "tika-s0401t18:1_0": "600-611.",
  "tika-s0401t18:1_1": "Natthi ettha mataṁ maraṇaṁ vināsoti amataṁ,",
  "tika-s0401t18:1_2": "nibbānanti āha –",
  "tika-s0401t18:1_3": "“maraṇavirahitaṁ nibbānaṁ paribhuñjantī”ti.",
  "tika-s0401t18:1_4": "Amatassa vā nibbānassa adhigamahetutāya amatasadisaatappakasukhapatitatāya ca kāyagatāsati “amata”nti vuttā.",
  "tika-s0401t18:1_5": "Paribhuñjantīti jhānasamāpajjanena vaḷañjanti.",
  "tika-s0401t18:1_6": "Viraddhanti anadhigamena virajjhitaṁ. Tenāha –",
  "tika-s0401t18:1_7": "“virādhitaṁ nādhigata”nti.",
  "tika-s0401t18:1_8": "Āraddhanti sādhitaṁ nipphāditaṁ.",
  "tika-s0401t18:1_9": "Tañca paripuṇṇaṁ nāma hotīti āha –",
  "tika-s0401t18:1_10": "“āraddhanti paripuṇṇa”nti.",
  "tika-s0401t18:1_11": "Pamādiṁsūti kālabyattayenedaṁ vuttanti āha –",
  "tika-s0401t18:1_12": "“pamajjantī”ti.",
  "tika-s0401t18:2_0": "Amatavaggavaṇṇanā niṭṭhitā.",
  "tika-s0401t18:3_0": "Iti manorathapūraṇiyā aṅguttaranikāya-aṭṭhakathāya",
  "tika-s0401t18:4_0": "Ekakanipātavaṇṇanāya anuttānatthadīpanā samattā.",
  "tika-s0401t18:5_0": "Paṭhamo bhāgo niṭṭhito."
}