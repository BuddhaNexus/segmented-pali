{
  "mil6.3.9:0.1_0": "Milindapañha",
  "mil6.3.9:0.2_0": "Anumānapañha",
  "mil6.3.9:0.3_0": "Vessantaravagga",
  "mil6.3.9:0.4_0": "9. Ekantasukhanibbānapañha",
  "mil6.3.9:1.1_0": "“Bhante nāgasena, kiṁ ekantasukhaṁ nibbānaṁ, udāhu dukkhena missan”ti?",
  "mil6.3.9:1.2_0": "“Ekantasukhaṁ, mahārāja, nibbānaṁ, dukkhena amissan”ti.",
  "mil6.3.9:2.1_0": "“Na mayaṁ taṁ,",
  "mil6.3.9:2.1_1": "bhante nāgasena,",
  "mil6.3.9:2.1_2": "vacanaṁ saddahāma ‘ekantasukhaṁ nibbānan’ti,",
  "mil6.3.9:2.1_3": "evamettha mayaṁ,",
  "mil6.3.9:2.1_4": "bhante nāgasena,",
  "mil6.3.9:2.1_5": "paccema ‘nibbānaṁ dukkhena missan’ti,",
  "mil6.3.9:2.1_6": "kāraṇañcettha upalabhāma ‘nibbānaṁ dukkhena missan’ti.",
  "mil6.3.9:2.2_0": "Katamaṁ ettha kāraṇaṁ?",
  "mil6.3.9:2.3_0": "Ye te,",
  "mil6.3.9:2.3_1": "bhante nāgasena,",
  "mil6.3.9:2.3_2": "nibbānaṁ pariyesanti,",
  "mil6.3.9:2.3_3": "tesaṁ dissati kāyassa ca cittassa ca ātāpo paritāpo ṭhānacaṅkamanisajjāsayanāhārapariggaho middhassa",
  "mil6.3.9:2.3_4": "ca uparodho āyatanānañca paṭipīḷanaṁ dhanadhaññapiyañātimittappajahanaṁ.",
  "mil6.3.9:2.4_0": "Ye keci loke sukhitā sukhasamappitā,",
  "mil6.3.9:2.4_1": "te sabbepi pañcahi kāmaguṇehi āyatane ramenti brūhenti,",
  "mil6.3.9:2.4_2": "manāpikamanāpikabahuvidhasubhanimittena rūpena cakkhuṁ ramenti brūhenti,",
  "mil6.3.9:2.4_3": "manāpikamanāpikagītavāditabahuvidhasubhanimittena saddena sotaṁ ramenti brūhenti,",
  "mil6.3.9:2.4_4": "manāpikamanāpikapupphaphalapattatacamūlasārabahuvidhasubhanimittena gandhena ghānaṁ ramenti brūhenti,",
  "mil6.3.9:2.4_5": "manāpikamanāpikakhajjabhojjaleyyapeyyasāyanīyabahuvidhasubhanimittena rasena jivhaṁ ramenti brūhenti,",
  "mil6.3.9:2.4_6": "manāpikamanāpikasaṇhasukhumamudumaddavabahuvidhasubhanimittena phassena kāyaṁ ramenti brūhenti,",
  "mil6.3.9:2.4_7": "manāpikamanāpikakalyāṇapāpakasubhāsubhabahuvidhavitakkamanasikārena manaṁ ramenti brūhenti.",
  "mil6.3.9:2.5_0": "Tumhe taṁ cakkhusotaghānajivhākāyamanobrūhanaṁ hanatha upahanatha,",
  "mil6.3.9:2.5_1": "chindatha upacchindatha,",
  "mil6.3.9:2.5_2": "rundhatha uparundhatha.",
  "mil6.3.9:2.6_0": "Tena kāyopi paritapati,",
  "mil6.3.9:2.6_1": "cittampi paritapati,",
  "mil6.3.9:2.6_2": "kāye paritatte kāyikadukkhavedanaṁ vediyati,",
  "mil6.3.9:2.6_3": "citte paritatte cetasikadukkhavedanaṁ vedayati.",
  "mil6.3.9:2.7_0": "Nanu māgaṇḍiyopi paribbājako bhagavantaṁ garahamāno evamāha ‘bhūnahu samaṇo gotamo’ti.",
  "mil6.3.9:2.8_0": "Idamettha kāraṇaṁ, yenāhaṁ kāraṇena brūmi ‘nibbānaṁ dukkhena missan’”ti.",
  "mil6.3.9:3.1_0": "“Na hi, mahārāja, nibbānaṁ dukkhena missaṁ, ekantasukhaṁ nibbānaṁ.",
  "mil6.3.9:3.2_0": "Yaṁ pana tvaṁ, mahārāja,",
  "mil6.3.9:3.2_1": "brūsi ‘nibbānaṁ dukkhan’ti,",
  "mil6.3.9:3.2_2": "netaṁ dukkhaṁ nibbānaṁ nāma,",
  "mil6.3.9:3.2_3": "nibbānassa pana sacchikiriyāya pubbabhāgo eso,",
  "mil6.3.9:3.2_4": "nibbānapariyesanaṁ etaṁ,",
  "mil6.3.9:3.2_5": "ekantasukhaṁyeva, mahārāja, nibbānaṁ,",
  "mil6.3.9:3.2_6": "na dukkhena missaṁ.",
  "mil6.3.9:3.3_0": "Ettha kāraṇaṁ vadāmi.",
  "mil6.3.9:3.4_0": "Atthi, mahārāja, rājūnaṁ rajjasukhaṁ nāmā”ti?",
  "mil6.3.9:3.5_0": "“Āma, bhante, atthi rājūnaṁ rajjasukhan”ti.",
  "mil6.3.9:3.6_0": "“Api nu kho taṁ, mahārāja, rajjasukhaṁ dukkhena missan”ti?",
  "mil6.3.9:3.7_0": "“Na hi, bhante”ti.",
  "mil6.3.9:3.8_0": "“Kissa pana te, mahārāja,",
  "mil6.3.9:3.8_1": "rājāno paccante kupite tesaṁ paccantanissitānaṁ paṭisedhāya amaccehi pariṇāyakehi bhaṭehi balatthehi",
  "mil6.3.9:3.8_2": "parivutā pavāsaṁ gantvā ḍaṁsamakasavātātapapaṭipīḷitā samavisame paridhāvanti,",
  "mil6.3.9:3.8_3": "mahāyuddhañca karonti,",
  "mil6.3.9:3.8_4": "jīvitasaṁsayañca pāpuṇantī”ti?",
  "mil6.3.9:3.9_0": "“Netaṁ,",
  "mil6.3.9:3.9_1": "bhante nāgasena,",
  "mil6.3.9:3.9_2": "rajjasukhaṁ nāma,",
  "mil6.3.9:3.9_3": "rajjasukhassa pariyesanāya pubbabhāgo eso, dukkhena,",
  "mil6.3.9:3.9_4": "bhante nāgasena,",
  "mil6.3.9:3.9_5": "rājāno rajjaṁ pariyesitvā rajjasukhaṁ anubhavanti, evaṁ,",
  "mil6.3.9:3.9_6": "bhante nāgasena,",
  "mil6.3.9:3.9_7": "rajjasukhaṁ dukkhena amissaṁ,",
  "mil6.3.9:3.9_8": "aññaṁ taṁ rajjasukhaṁ,",
  "mil6.3.9:3.9_9": "aññaṁ dukkhan”ti.",
  "mil6.3.9:3.10_0": "“Evameva kho, mahārāja, ekantasukhaṁ nibbānaṁ, na dukkhena missaṁ.",
  "mil6.3.9:3.11_0": "Ye pana taṁ nibbānaṁ pariyesanti,",
  "mil6.3.9:3.11_1": "te kāyañca cittañca ātāpetvā ṭhānacaṅkamanisajjāsayanāhāraṁ pariggahetvā middhaṁ uparundhitvā āyatanāni",
  "mil6.3.9:3.11_2": "paṭipīḷetvā kāyañca jīvitañca pariccajitvā dukkhena nibbānaṁ pariyesitvā ekantasukhaṁ nibbānaṁ anubhavanti,",
  "mil6.3.9:3.11_3": "nihatapaccāmittā viya rājāno rajjasukhaṁ.",
  "mil6.3.9:3.12_0": "Evaṁ, mahārāja, ekantasukhaṁ nibbānaṁ, na dukkhena missaṁ, aññaṁ nibbānaṁ, aññaṁ dukkhanti.",
  "mil6.3.9:4.1_0": "Aparampi, mahārāja,",
  "mil6.3.9:4.1_1": "uttariṁ kāraṇaṁ suṇohi ekantasukhaṁ nibbānaṁ,",
  "mil6.3.9:4.1_2": "na dukkhena missaṁ,",
  "mil6.3.9:4.1_3": "aññaṁ dukkhaṁ,",
  "mil6.3.9:4.1_4": "aññaṁ nibbānanti.",
  "mil6.3.9:4.2_0": "Atthi, mahārāja, ācariyānaṁ sippavantānaṁ sippasukhaṁ nāmā”ti?",
  "mil6.3.9:4.3_0": "“Āma, bhante, atthi ācariyānaṁ sippavantānaṁ sippasukhan”ti.",
  "mil6.3.9:4.4_0": "“Api nu kho taṁ, mahārāja, sippasukhaṁ dukkhena missan”ti?",
  "mil6.3.9:4.5_0": "“Na hi, bhante”ti.",
  "mil6.3.9:4.6_0": "“Kissa pana te, mahārāja,",
  "mil6.3.9:4.6_1": "ācariyā ācariyānaṁ abhivādanapaccuṭṭhānena udakāharaṇagharasammajjanadantakaṭṭhamukhodakānuppadānena",
  "mil6.3.9:4.6_2": "ucchiṭṭhapaṭiggahaṇaucchādananahāpanapādaparikammena sakacittaṁ nikkhipitvā paracittānuvattanena dukkhaseyyāya",
  "mil6.3.9:4.6_3": "visamabhojanena kāyaṁ ātāpentī”ti?",
  "mil6.3.9:4.7_0": "“Netaṁ,",
  "mil6.3.9:4.7_1": "bhante nāgasena,",
  "mil6.3.9:4.7_2": "sippasukhaṁ nāma,",
  "mil6.3.9:4.7_3": "sippapariyesanāya pubbabhāgo eso, dukkhena,",
  "mil6.3.9:4.7_4": "bhante nāgasena,",
  "mil6.3.9:4.7_5": "ācariyā sippaṁ pariyesitvā sippasukhaṁ anubhavanti, evaṁ,",
  "mil6.3.9:4.7_6": "bhante nāgasena,",
  "mil6.3.9:4.7_7": "sippasukhaṁ dukkhena amissaṁ,",
  "mil6.3.9:4.7_8": "aññaṁ taṁ sippasukhaṁ,",
  "mil6.3.9:4.7_9": "aññaṁ dukkhan”ti.",
  "mil6.3.9:4.8_0": "“Evameva kho, mahārāja, ekantasukhaṁ nibbānaṁ, na dukkhena missaṁ.",
  "mil6.3.9:4.9_0": "Ye pana taṁ nibbānaṁ pariyesanti,",
  "mil6.3.9:4.9_1": "te kāyañca cittañca ātāpetvā ṭhānacaṅkamanisajjāsayanāhāraṁ pariggahetvā middhaṁ uparundhitvā āyatanāni",
  "mil6.3.9:4.9_2": "paṭipīḷetvā kāyañca jīvitañca pariccajitvā dukkhena nibbānaṁ pariyesitvā ekantasukhaṁ nibbānaṁ anubhavanti,",
  "mil6.3.9:4.9_3": "ācariyā viya sippasukhaṁ.",
  "mil6.3.9:4.10_0": "Evaṁ, mahārāja, ekantasukhaṁ nibbānaṁ, na dukkhena missaṁ, aññaṁ dukkhaṁ, aññaṁ nibbānan”ti.",
  "mil6.3.9:4.11_0": "“Sādhu, bhante nāgasena, evametaṁ tathā sampaṭicchāmī”ti.",
  "mil6.3.9:5.1_0": "Ekantasukhanibbānapañho navamo."
}