{
  "pli-tv-bi-vb-np12:0.1_0": "Theravāda Vinaya",
  "pli-tv-bi-vb-np12:0.2_0": "Bhikkhunivibhaṅga",
  "pli-tv-bi-vb-np12:0.3_0": "Nissaggiyakaṇḍa",
  "pli-tv-bi-vb-np12:0.4_0": "12. Cīvaravagga",
  "pli-tv-bi-vb-np12:0.5_0": "2. Lahupāvuraṇasikkhāpada",
  "pli-tv-bi-vb-np12:1.1_0": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bi-vb-np12:1.2_0": "Tena kho pana samayena thullanandā bhikkhunī bahussutā hoti bhāṇikā visāradā paṭṭā dhammiṁ kathaṁ kātuṁ.",
  "pli-tv-bi-vb-np12:1.3_0": "Atha kho rājā pasenadi kosalo uṇhakāle mahagghaṁ khomaṁ pārupitvā yena thullanandā bhikkhunī tenupasaṅkami;",
  "pli-tv-bi-vb-np12:1.3_1": "upasaṅkamitvā thullanandaṁ bhikkhuniṁ abhivādetvā ekamantaṁ nisīdi.",
  "pli-tv-bi-vb-np12:1.4_0": "Ekamantaṁ nisinnaṁ kho rājānaṁ pasenadiṁ kosalaṁ thullanandā bhikkhunī dhammiyā kathāya sandassesi samādapesi samuttejesi sampahaṁsesi.",
  "pli-tv-bi-vb-np12:1.5_0": "Atha kho rājā pasenadi kosalo thullanandāya bhikkhuniyā dhammiyā kathāya sandassito samādapito samuttejito sampahaṁsito thullanandaṁ bhikkhuniṁ etadavoca—",
  "pli-tv-bi-vb-np12:1.6_0": "“vadeyyāsi, ayye, yena attho”ti.",
  "pli-tv-bi-vb-np12:1.7_0": "“Sace me tvaṁ, mahārāja, dātukāmosi, imaṁ khomaṁ dehī”ti.",
  "pli-tv-bi-vb-np12:1.8_0": "Atha kho rājā pasenadi kosalo thullanandāya bhikkhuniyā khomaṁ datvā uṭṭhāyāsanā thullanandaṁ bhikkhuniṁ abhivādetvā padakkhiṇaṁ katvā pakkāmi.",
  "pli-tv-bi-vb-np12:1.9_0": "Manussā ujjhāyanti khiyyanti vipācenti—",
  "pli-tv-bi-vb-np12:1.10_0": "“mahicchā imā bhikkhuniyo asantuṭṭhā.",
  "pli-tv-bi-vb-np12:1.11_0": "Kathañhi nāma rājānaṁ khomaṁ viññāpessantī”ti.",
  "pli-tv-bi-vb-np12:1.12_0": "Assosuṁ kho bhikkhuniyo tesaṁ manussānaṁ ujjhāyantānaṁ khiyyantānaṁ vipācentānaṁ.",
  "pli-tv-bi-vb-np12:1.13_0": "Yā tā bhikkhuniyo appicchā …pe… tā ujjhāyanti khiyyanti vipācenti—",
  "pli-tv-bi-vb-np12:1.14_0": "“kathañhi nāma ayyā thullanandā rājānaṁ khomaṁ viññāpessatī”ti …pe…",
  "pli-tv-bi-vb-np12:1.15_0": "“saccaṁ kira, bhikkhave,",
  "pli-tv-bi-vb-np12:1.15_1": "thullanandā bhikkhunī rājānaṁ khomaṁ viññāpetī”ti?",
  "pli-tv-bi-vb-np12:1.16_0": "“Saccaṁ, bhagavā”ti.",
  "pli-tv-bi-vb-np12:1.17_0": "Vigarahi buddho bhagavā …pe…",
  "pli-tv-bi-vb-np12:1.18_0": "kathañhi nāma, bhikkhave,",
  "pli-tv-bi-vb-np12:1.18_1": "thullanandā bhikkhunī rājānaṁ khomaṁ viññāpessati.",
  "pli-tv-bi-vb-np12:1.19_0": "Netaṁ, bhikkhave, appasannānaṁ vā pasādāya …pe…",
  "pli-tv-bi-vb-np12:1.20_0": "evañca pana, bhikkhave,",
  "pli-tv-bi-vb-np12:1.20_1": "bhikkhuniyo imaṁ sikkhāpadaṁ uddisantu—",
  "pli-tv-bi-vb-np12:1.21.1_0": "“Lahupāvuraṇaṁ pana bhikkhuniyā cetāpentiyā aḍḍhateyyakaṁsaparamaṁ cetāpetabbaṁ. Tato ce uttari cetāpeyya, nissaggiyaṁ pācittiyan”ti.",
  "pli-tv-bi-vb-np12:2.1.1_0": "Lahupāvuraṇaṁ nāma",
  "pli-tv-bi-vb-np12:2.1.2_0": "yaṁ kiñci uṇhakāle pāvuraṇaṁ.",
  "pli-tv-bi-vb-np12:2.1.3_0": "Cetāpentiyāti",
  "pli-tv-bi-vb-np12:2.1.4_0": "viññāpentiyā.",
  "pli-tv-bi-vb-np12:2.1.5_0": "Aḍḍhateyyakaṁsaparamaṁ cetāpetabbanti",
  "pli-tv-bi-vb-np12:2.1.6_0": "dasakahāpaṇagghanakaṁ cetāpetabbaṁ.",
  "pli-tv-bi-vb-np12:2.1.7_0": "Tato ce uttari cetāpeyyāti",
  "pli-tv-bi-vb-np12:2.1.8_0": "tatuttari viññāpeti, payoge dukkaṭaṁ.",
  "pli-tv-bi-vb-np12:2.1.9_0": "Paṭilābhena nissaggiyaṁ hoti.",
  "pli-tv-bi-vb-np12:2.1.10_0": "Nissajjitabbaṁ saṅghassa vā gaṇassa vā ekabhikkhuniyā vā.",
  "pli-tv-bi-vb-np12:2.1.11_0": "Evañca pana, bhikkhave, nissajjitabbaṁ.",
  "pli-tv-bi-vb-np12:2.1.12_0": "…pe…",
  "pli-tv-bi-vb-np12:2.1.13_0": "“idaṁ me, ayye,",
  "pli-tv-bi-vb-np12:2.1.13_1": "lahupāvuraṇaṁ atirekaaḍḍhateyyakaṁsaparamaṁ cetāpitaṁ nissaggiyaṁ,",
  "pli-tv-bi-vb-np12:2.1.13_2": "imāhaṁ saṅghassa nissajjāmī”ti …pe…",
  "pli-tv-bi-vb-np12:2.1.14_0": "dadeyyā”ti …pe…",
  "pli-tv-bi-vb-np12:2.1.15_0": "dadeyyun”ti …pe…",
  "pli-tv-bi-vb-np12:2.1.16_0": "ayyāya dammī”ti.",
  "pli-tv-bi-vb-np12:2.1.17.1_0": "Atirekaaḍḍhateyyakaṁse atirekasaññā cetāpeti, nissaggiyaṁ pācittiyaṁ.",
  "pli-tv-bi-vb-np12:2.1.18_0": "Atirekaaḍḍhateyyakaṁse vematikā cetāpeti, nissaggiyaṁ pācittiyaṁ.",
  "pli-tv-bi-vb-np12:2.1.19_0": "Atirekaaḍḍhateyyakaṁse ūnakasaññā cetāpeti, nissaggiyaṁ pācittiyaṁ.",
  "pli-tv-bi-vb-np12:2.1.20_0": "Ūnakaaḍḍhateyyakaṁse atirekasaññā, āpatti dukkaṭassa.",
  "pli-tv-bi-vb-np12:2.1.21_0": "Ūnakaaḍḍhateyyakaṁse vematikā, āpatti dukkaṭassa.",
  "pli-tv-bi-vb-np12:2.1.22_0": "Ūnakaaḍḍhateyyakaṁse ūnakasaññā, anāpatti.",
  "pli-tv-bi-vb-np12:2.2.1_0": "Anāpatti—",
  "pli-tv-bi-vb-np12:2.2.2_0": "aḍḍhateyyakaṁsaparamaṁ cetāpeti,",
  "pli-tv-bi-vb-np12:2.2.3_0": "ūnakaaḍḍhateyyakaṁsaparamaṁ cetāpeti,",
  "pli-tv-bi-vb-np12:2.2.4_0": "ñātakānaṁ,",
  "pli-tv-bi-vb-np12:2.2.5_0": "pavāritānaṁ,",
  "pli-tv-bi-vb-np12:2.2.6_0": "aññassatthāya,",
  "pli-tv-bi-vb-np12:2.2.7_0": "attano dhanena,",
  "pli-tv-bi-vb-np12:2.2.8_0": "mahagghaṁ cetāpetukāmassa appagghaṁ cetāpeti,",
  "pli-tv-bi-vb-np12:2.2.9_0": "ummattikāya,",
  "pli-tv-bi-vb-np12:2.2.10_0": "ādikammikāyāti.",
  "pli-tv-bi-vb-np12:2.2.11_0": "Dvādasamasikkhāpadaṁ niṭṭhitaṁ.",
  "pli-tv-bi-vb-np12:2.2.12.0_0": "(Dutiya) Pattavagga",
  "pli-tv-bi-vb-np12:2.2.13.0_0": "10 Pariṇatasikkhāpada",
  "pli-tv-bi-vb-np12:2.2.14_0": "Uddiṭṭhā kho, ayyāyo,",
  "pli-tv-bi-vb-np12:2.2.14_1": "tiṁsa nissaggiyā pācittiyā dhammā.",
  "pli-tv-bi-vb-np12:2.2.15_0": "Tatthāyyāyo pucchāmi—",
  "pli-tv-bi-vb-np12:2.2.16_0": "“kaccittha parisuddhā”?",
  "pli-tv-bi-vb-np12:2.2.17_0": "Dutiyampi pucchāmi—",
  "pli-tv-bi-vb-np12:2.2.18_0": "“kaccittha parisuddhā”?",
  "pli-tv-bi-vb-np12:2.2.19_0": "Tatiyampi pucchāmi—",
  "pli-tv-bi-vb-np12:2.2.20_0": "“kaccittha parisuddhā”?",
  "pli-tv-bi-vb-np12:2.2.21_0": "Parisuddhetthāyyāyo, tasmā tuṇhī, evametaṁ dhārayāmīti.",
  "pli-tv-bi-vb-np12:2.2.22_0": "Bhikkhunivibhaṅge nissaggiyakaṇḍaṁ niṭṭhitaṁ."
}