{
  "tha-ap169:0.1_0": "Therāpadāna",
  "tha-ap169:0.2_0": "Supāricariyavagga",
  "tha-ap169:0.3_0": "7 Yūthikapupphiyattheraapadāna",
  "tha-ap169:1.1_0": "“Candabhāgānadītīre,",
  "tha-ap169:1.2_0": "anusotaṁ vajāmahaṁ;",
  "tha-ap169:1.3_0": "Sayambhuṁ addasaṁ tattha,",
  "tha-ap169:1.4_0": "sālarājaṁva phullitaṁ.",
  "tha-ap169:2.1_0": "Pupphaṁ yūthikamādāya,",
  "tha-ap169:2.2_0": "upagacchiṁ mahāmuniṁ;",
  "tha-ap169:2.3_0": "Pasannacitto sumano,",
  "tha-ap169:2.4_0": "buddhassa abhiropayiṁ.",
  "tha-ap169:3.1_0": "Catunnavutito kappe,",
  "tha-ap169:3.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap169:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap169:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap169:4.1_0": "Sattasaṭṭhimhito kappe,",
  "tha-ap169:4.2_0": "eko sāmuddharo ahuṁ;",
  "tha-ap169:4.3_0": "Sattaratanasampanno,",
  "tha-ap169:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap169:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap169:5.2_0": "…pe…",
  "tha-ap169:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap169:6.1_0": "Itthaṁ sudaṁ āyasmā yūthikapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap169:7.1_0": "Yūthikapupphiyattherassāpadānaṁ sattamaṁ."
}