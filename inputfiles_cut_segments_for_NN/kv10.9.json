{
  "kv10.9:0.1_0": "Kathāvatthu",
  "kv10.9:0.2_0": "Mahāpaṇṇāsaka",
  "kv10.9:0.3_0": "Dasamavagga",
  "kv10.9:0.4_0": "Samādānahetukathā",
  "kv10.9:1.1_0": "Samādānahetukaṁ sīlaṁ vaḍḍhatīti?",
  "kv10.9:1.2_0": "Āmantā.",
  "kv10.9:1.3_0": "Samādānahetuko phasso vaḍḍhati,",
  "kv10.9:1.3_1": "vedanā vaḍḍhati,",
  "kv10.9:1.3_2": "saññā vaḍḍhati,",
  "kv10.9:1.3_3": "cetanā vaḍḍhati,",
  "kv10.9:1.3_4": "cittaṁ vaḍḍhati,",
  "kv10.9:1.3_5": "saddhā vaḍḍhati,",
  "kv10.9:1.3_6": "vīriyaṁ vaḍḍhati,",
  "kv10.9:1.3_7": "sati vaḍḍhati,",
  "kv10.9:1.3_8": "samādhi vaḍḍhati,",
  "kv10.9:1.3_9": "paññā vaḍḍhatīti?",
  "kv10.9:1.4_0": "Na hevaṁ vattabbe …pe….",
  "kv10.9:2.1_0": "Samādānahetukaṁ sīlaṁ vaḍḍhatīti?",
  "kv10.9:2.2_0": "Āmantā.",
  "kv10.9:2.3_0": "Latā viya vaḍḍhati,",
  "kv10.9:2.3_1": "māluvā viya vaḍḍhati,",
  "kv10.9:2.3_2": "rukkho viya vaḍḍhati,",
  "kv10.9:2.3_3": "tiṇaṁ viya vaḍḍhati,",
  "kv10.9:2.3_4": "muñjapuñjo viya vaḍḍhatīti?",
  "kv10.9:2.4_0": "Na hevaṁ vattabbe …pe….",
  "kv10.9:3.1_0": "Samādānahetukaṁ sīlaṁ vaḍḍhatīti?",
  "kv10.9:3.2_0": "Āmantā.",
  "kv10.9:3.3_0": "Sīlaṁ samādiyitvā kāmavitakkaṁ vitakkentassa byāpādavitakkaṁ vitakkentassa vihiṁsāvitakkaṁ vitakkentassa sīlaṁ vaḍḍhatīti?",
  "kv10.9:3.4_0": "Āmantā.",
  "kv10.9:3.5_0": "Dvinnaṁ phassānaṁ …pe… dvinnaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv10.9:3.6_0": "Na hevaṁ vattabbe …pe… dvinnaṁ phassānaṁ …pe… dvinnaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv10.9:3.7_0": "Āmantā.",
  "kv10.9:3.8_0": "Kusalākusalā sāvajjānavajjā hīnapaṇītā kaṇhasukkasappaṭibhāgā dhammā sammukhībhāvaṁ āgacchantīti?",
  "kv10.9:3.9_0": "Na hevaṁ vattabbe …pe….",
  "kv10.9:4.1_0": "Kusalākusalā sāvajjānavajjā hīnapaṇītā kaṇhasukkasappaṭibhāgā dhammā sammukhībhāvaṁ āgacchantīti?",
  "kv10.9:4.2_0": "Āmantā.",
  "kv10.9:4.3_0": "Nanu vuttaṁ bhagavatā—",
  "kv10.9:4.4_0": "“cattārimāni, bhikkhave, suvidūravidūrāni.",
  "kv10.9:4.5_0": "Katamāni cattāri?",
  "kv10.9:4.6_0": "Nabhañca, bhikkhave, pathavī ca—",
  "kv10.9:4.7_0": "idaṁ paṭhamaṁ suvidūravidūraṁ …pe… tasmā sataṁ dhammo asabbhi ārakā”ti.",
  "kv10.9:4.8_0": "Attheva suttantoti?",
  "kv10.9:4.9_0": "Āmantā.",
  "kv10.9:4.10_0": "Tena hi na vattabbaṁ—",
  "kv10.9:4.11_0": "“kusalākusalā sāvajjānavajjā hīnapaṇītā kaṇhasukkasappaṭibhāgā dhammā sammukhībhāvaṁ āgacchantī”ti.",
  "kv10.9:5.1_0": "Na vattabbaṁ—",
  "kv10.9:5.2_0": "“samādānahetukaṁ sīlaṁ vaḍḍhatī”ti?",
  "kv10.9:5.3_0": "Āmantā.",
  "kv10.9:5.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv10.9:5.5_0": "“ārāmaropā vanaropā …pe…",
  "kv10.9:5.6_0": "dhammaṭṭhā sīlasampannā te janā saggagāmino”ti.",
  "kv10.9:5.7_0": "Attheva suttantoti?",
  "kv10.9:5.8_0": "Āmantā.",
  "kv10.9:5.9_0": "Tena hi samādānahetukaṁ sīlaṁ vaḍḍhatīti.",
  "kv10.9:5.10_0": "Samādānahetukathā niṭṭhitā."
}