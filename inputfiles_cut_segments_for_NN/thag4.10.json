{
  "thag4.10:0.1_0": "Theragāthā",
  "thag4.10:0.2_0": "Catukkanipāta",
  "thag4.10:0.3_0": "Paṭhamavagga",
  "thag4.10:0.4_0": "4.10. Dhammikattheragāthā",
  "thag4.10:1.1_0": "“Dhammo have rakkhati dhammacāriṁ,",
  "thag4.10:1.2_0": "Dhammo suciṇṇo sukhamāvahati;",
  "thag4.10:1.3_0": "Esānisaṁso dhamme suciṇṇe,",
  "thag4.10:1.4_0": "Na duggatiṁ gacchati dhammacārī.",
  "thag4.10:2.1_0": "Na hi dhammo adhammo ca,",
  "thag4.10:2.2_0": "ubho samavipākino;",
  "thag4.10:2.3_0": "Adhammo nirayaṁ neti,",
  "thag4.10:2.4_0": "dhammo pāpeti suggatiṁ.",
  "thag4.10:3.1_0": "Tasmā hi dhammesu kareyya chandaṁ,",
  "thag4.10:3.2_0": "Iti modamāno sugatena tādinā;",
  "thag4.10:3.3_0": "Dhamme ṭhitā sugatavarassa sāvakā,",
  "thag4.10:3.4_0": "Nīyanti dhīrā saraṇavaraggagāmino”.",
  "thag4.10:4.1_0": "“Vipphoṭito gaṇḍamūlo,",
  "thag4.10:4.2_0": "Taṇhājālo samūhato;",
  "thag4.10:4.3_0": "So khīṇasaṁsāro na catthi kiñcanaṁ,",
  "thag4.10:4.4_0": "Cando yathā dosinā puṇṇamāsiyan”ti.",
  "thag4.10:4.5_0": "… Dhammiko thero …."
}