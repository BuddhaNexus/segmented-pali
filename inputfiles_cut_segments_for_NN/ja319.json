{
  "ja319:0.1_0": "Jātaka",
  "ja319:0.2_0": "Catukkanipāta",
  "ja319:0.3_0": "Pucimandavagga",
  "ja319:0.4_0": "9. Tittirajātaka",
  "ja319:1.1_0": "“Susukhaṁ vata jīvāmi,",
  "ja319:1.2_0": "labhāmi ceva bhuñjituṁ;",
  "ja319:1.3_0": "Paripanthe ca tiṭṭhāmi,",
  "ja319:1.4_0": "kā nu bhante gatī mama”.",
  "ja319:2.1_0": "“Mano ce te nappaṇamati,",
  "ja319:2.2_0": "pakkhi pāpassa kammuno;",
  "ja319:2.3_0": "Abyāvaṭassa bhadrassa,",
  "ja319:2.4_0": "na pāpamupalimpati”.",
  "ja319:3.1_0": "“Ñātako no nisinnoti,",
  "ja319:3.2_0": "bahu āgacchate jano;",
  "ja319:3.3_0": "Paṭicca kammaṁ phusati,",
  "ja319:3.4_0": "tasmiṁ me saṅkate mano”.",
  "ja319:4.1_0": "“Na paṭicca kammaṁ phusati,",
  "ja319:4.2_0": "mano ce nappadussati;",
  "ja319:4.3_0": "Appossukkassa bhadrassa,",
  "ja319:4.4_0": "na pāpamupalimpatī”ti.",
  "ja319:5.1_0": "Tittirajātakaṁ navamaṁ."
}