{
  "kv18.3:0.1_0": "Kathāvatthu",
  "kv18.3:0.2_0": "Mahāpaṇṇāsaka",
  "kv18.3:0.3_0": "Aṭṭhārasamavagga",
  "kv18.3:0.4_0": "Karuṇākathā",
  "kv18.3:1.1_0": "Natthi buddhassa bhagavato karuṇāti?",
  "kv18.3:1.2_0": "Āmantā.",
  "kv18.3:1.3_0": "Natthi buddhassa bhagavato mettāti?",
  "kv18.3:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv18.3:1.5_0": "natthi buddhassa bhagavato karuṇāti?",
  "kv18.3:1.6_0": "Āmantā.",
  "kv18.3:1.7_0": "Natthi buddhassa bhagavato muditā …pe…",
  "kv18.3:1.8_0": "upekkhāti?",
  "kv18.3:1.9_0": "Na hevaṁ vattabbe …pe….",
  "kv18.3:2.1_0": "Atthi buddhassa bhagavato mettāti?",
  "kv18.3:2.2_0": "Āmantā.",
  "kv18.3:2.3_0": "Atthi buddhassa bhagavato karuṇāti?",
  "kv18.3:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv18.3:2.5_0": "atthi buddhassa bhagavato muditā …pe…",
  "kv18.3:2.6_0": "upekkhāti?",
  "kv18.3:2.7_0": "Āmantā.",
  "kv18.3:2.8_0": "Atthi buddhassa bhagavato karuṇāti?",
  "kv18.3:2.9_0": "Na hevaṁ vattabbe …pe….",
  "kv18.3:3.1_0": "Natthi buddhassa bhagavato karuṇāti?",
  "kv18.3:3.2_0": "Āmantā.",
  "kv18.3:3.3_0": "Bhagavā akāruṇikoti?",
  "kv18.3:3.4_0": "Na hevaṁ vattabbe …pe…",
  "kv18.3:3.5_0": "nanu bhagavā kāruṇiko lokahito lokānukampako lokatthacaroti?",
  "kv18.3:3.6_0": "Āmantā.",
  "kv18.3:3.7_0": "Hañci bhagavā kāruṇiko lokahito lokānukampako lokatthacaro, no ca vata re vattabbe—",
  "kv18.3:3.8_0": "“natthi buddhassa bhagavato karuṇā”ti …pe….",
  "kv18.3:4.1_0": "Natthi buddhassa bhagavato karuṇāti?",
  "kv18.3:4.2_0": "Āmantā.",
  "kv18.3:4.3_0": "Nanu bhagavā mahākaruṇāsamāpattiṁ samāpajjīti?",
  "kv18.3:4.4_0": "Āmantā.",
  "kv18.3:4.5_0": "Hañci bhagavā mahākaruṇāsamāpattiṁ samāpajji, no ca vata re vattabbe—",
  "kv18.3:4.6_0": "“natthi buddhassa bhagavato karuṇā”ti.",
  "kv18.3:5.1_0": "Atthi buddhassa bhagavato karuṇāti?",
  "kv18.3:5.2_0": "Āmantā.",
  "kv18.3:5.3_0": "Bhagavā sarāgoti?",
  "kv18.3:5.4_0": "Na hevaṁ vattabbe.",
  "kv18.3:5.5_0": "Tena hi natthi buddhassa bhagavato karuṇāti.",
  "kv18.3:5.6_0": "Karuṇākathā niṭṭhitā."
}