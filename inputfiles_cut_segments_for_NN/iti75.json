{
  "iti75:0.1_0": "Itivuttaka 75",
  "iti75:0.2_0": "Tikanipāta",
  "iti75:0.3_0": "Tatiyavagga",
  "iti75:0.4_0": "Avuṭṭhikasutta",
  "iti75:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti75:2.1_0": "“Tayome, bhikkhave, puggalā santo saṁvijjamānā lokasmiṁ.",
  "iti75:2.2_0": "Katame tayo?",
  "iti75:2.3_0": "Avuṭṭhikasamo, padesavassī, sabbatthābhivassī.",
  "iti75:3.1_0": "Kathañca, bhikkhave, puggalo avuṭṭhikasamo hoti?",
  "iti75:3.2_0": "Idha,",
  "iti75:3.2_1": "bhikkhave,",
  "iti75:3.2_2": "ekacco puggalo sabbesaññeva na dātā hoti,",
  "iti75:3.2_3": "samaṇabrāhmaṇakapaṇaddhikavanibbakayācakānaṁ annaṁ pānaṁ vatthaṁ yānaṁ mālāgandhavilepanaṁ seyyāvasathapadīpeyyaṁ.",
  "iti75:3.3_0": "Evaṁ kho, bhikkhave, puggalo avuṭṭhikasamo hoti.",
  "iti75:4.1_0": "Kathañca, bhikkhave, puggalo padesavassī hoti?",
  "iti75:4.2_0": "Idha,",
  "iti75:4.2_1": "bhikkhave,",
  "iti75:4.2_2": "ekacco puggalo ekaccānaṁ dātā hoti,",
  "iti75:4.2_3": "ekaccānaṁ na dātā hoti samaṇabrāhmaṇakapaṇaddhikavanibbakayācakānaṁ annaṁ pānaṁ vatthaṁ yānaṁ mālāgandhavilepanaṁ seyyāvasathapadīpeyyaṁ.",
  "iti75:4.3_0": "Evaṁ kho, bhikkhave, puggalo padesavassī hoti.",
  "iti75:5.1_0": "Kathañca, bhikkhave, puggalo sabbatthābhivassī hoti?",
  "iti75:5.2_0": "Idha,",
  "iti75:5.2_1": "bhikkhave,",
  "iti75:5.2_2": "ekacco puggalo sabbesaṁva deti,",
  "iti75:5.2_3": "samaṇabrāhmaṇakapaṇaddhikavanibbakayācakānaṁ annaṁ pānaṁ vatthaṁ yānaṁ mālāgandhavilepanaṁ seyyāvasathapadīpeyyaṁ.",
  "iti75:5.3_0": "Evaṁ kho, bhikkhave, puggalo sabbatthābhivassī hoti.",
  "iti75:5.4_0": "Ime kho, bhikkhave, tayo puggalā santo saṁvijjamānā lokasmin”ti.",
  "iti75:5.5_0": "Etamatthaṁ bhagavā avoca.",
  "iti75:5.6_0": "Tatthetaṁ iti vuccati:",
  "iti75:6.1_0": "“Na samaṇe na brāhmaṇe,",
  "iti75:6.2_0": "Na kapaṇaddhikavanibbake;",
  "iti75:6.3_0": "Laddhāna saṁvibhājeti,",
  "iti75:6.4_0": "Annaṁ pānañca bhojanaṁ;",
  "iti75:6.5_0": "Taṁ ve avuṭṭhikasamoti,",
  "iti75:6.6_0": "Āhu naṁ purisādhamaṁ.",
  "iti75:7.1_0": "Ekaccānaṁ na dadāti,",
  "iti75:7.2_0": "ekaccānaṁ pavecchati;",
  "iti75:7.3_0": "Taṁ ve padesavassīti,",
  "iti75:7.4_0": "āhu medhāvino janā.",
  "iti75:8.1_0": "Subhikkhavāco puriso,",
  "iti75:8.2_0": "sabbabhūtānukampako;",
  "iti75:8.3_0": "Āmodamāno pakireti,",
  "iti75:8.4_0": "detha dethāti bhāsati.",
  "iti75:9.1_0": "Yathāpi megho thanayitvā,",
  "iti75:9.2_0": "gajjayitvā pavassati;",
  "iti75:9.3_0": "Thalaṁ ninnañca pūreti,",
  "iti75:9.4_0": "abhisandantova vārinā.",
  "iti75:10.1_0": "Evamevaṁ idhekacco,",
  "iti75:10.2_0": "puggalo hoti tādiso;",
  "iti75:10.3_0": "Dhammena saṁharitvāna,",
  "iti75:10.4_0": "uṭṭhānādhigataṁ dhanaṁ;",
  "iti75:10.5_0": "Tappeti annapānena,",
  "iti75:10.6_0": "sammā patte vanibbake”ti.",
  "iti75:11.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti75:11.2_0": "Chaṭṭhaṁ."
}