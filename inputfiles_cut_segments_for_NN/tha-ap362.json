{
  "tha-ap362:0.1_0": "Therāpadāna",
  "tha-ap362:0.2_0": "Saddasaññakavagga",
  "tha-ap362:0.3_0": "10. Piṇḍapātikattheraapadāna",
  "tha-ap362:1.1_0": "“Tisso nāmāsi sambuddho,",
  "tha-ap362:1.2_0": "vihāsi vipine tadā;",
  "tha-ap362:1.3_0": "Tusitā hi idhāgantvā,",
  "tha-ap362:1.4_0": "piṇḍapātaṁ adāsahaṁ.",
  "tha-ap362:2.1_0": "Sambuddhamabhivādetvā,",
  "tha-ap362:2.2_0": "tissaṁ nāma mahāyasaṁ;",
  "tha-ap362:2.3_0": "Sakaṁ cittaṁ pasādetvā,",
  "tha-ap362:2.4_0": "tusitaṁ agamāsahaṁ.",
  "tha-ap362:3.1_0": "Dvenavute ito kappe,",
  "tha-ap362:3.2_0": "yaṁ dānamadadiṁ tadā;",
  "tha-ap362:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap362:3.4_0": "piṇḍapātassidaṁ phalaṁ.",
  "tha-ap362:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap362:4.2_0": "…pe…",
  "tha-ap362:4.3_0": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap362:5.1_0": "Itthaṁ sudaṁ āyasmā piṇḍapātiko thero imā gāthāyo abhāsitthāti.",
  "tha-ap362:6.1_0": "Piṇḍapātikattherassāpadānaṁ dasamaṁ.",
  "tha-ap362:7.1_0": "Saddasaññakavaggo chattiṁsatimo.",
  "tha-ap362:8.1_0": "Tassuddānaṁ",
  "tha-ap362:9.1_0": "Saddasaññī yavasiko,",
  "tha-ap362:9.2_0": "kiṁsukoraṇḍapupphiyo;",
  "tha-ap362:9.3_0": "Ālambano ambayāgu,",
  "tha-ap362:9.4_0": "supuṭī mañcadāyako;",
  "tha-ap362:9.5_0": "Saraṇaṁ piṇḍapāto ca,",
  "tha-ap362:9.6_0": "gāthā tālīsameva ca."
}