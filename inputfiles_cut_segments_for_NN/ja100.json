{
  "ja100:0.1_0": "Jātaka",
  "ja100:0.2_0": "Ekakanipāta",
  "ja100:0.3_0": "Littavagga",
  "ja100:0.4_0": "10. Asātarūpajātaka",
  "ja100:1.1_0": "“Asātaṁ sātarūpena,",
  "ja100:1.2_0": "piyarūpena appiyaṁ;",
  "ja100:1.3_0": "Dukkhaṁ sukhassa rūpena,",
  "ja100:1.4_0": "pamattamativattatī”ti.",
  "ja100:2.1_0": "Asātarūpajātakaṁ dasamaṁ.",
  "ja100:2.2_0": "Littavaggo dasamo.",
  "ja100:3.1_0": "Tassuddānaṁ",
  "ja100:4.1_0": "Gilamakkhakutūhala mātukassā,",
  "ja100:4.2_0": "Muninā ca aniccata pattavaraṁ;",
  "ja100:4.3_0": "Dhanapālivaro atipaṇḍitako,",
  "ja100:4.4_0": "Saparosahassaasātadasāti.",
  "ja100:5.1_0": "Majjhimo paṇṇāsako."
}