{
  "kv4.3:0.1_0": "Kathāvatthu",
  "kv4.3:0.2_0": "Mahāpaṇṇāsaka",
  "kv4.3:0.3_0": "Catutthavagga",
  "kv4.3:0.4_0": "Anāsavakathā",
  "kv4.3:1.1_0": "Arahato sabbe dhammā anāsavāti?",
  "kv4.3:1.2_0": "Āmantā.",
  "kv4.3:1.3_0": "Maggo phalaṁ nibbānaṁ,",
  "kv4.3:1.3_1": "sotāpattimaggo sotāpattiphalaṁ,",
  "kv4.3:1.3_2": "sakadāgāmimaggo sakadāgāmiphalaṁ, anāgāmimaggo anāgāmiphalaṁ,",
  "kv4.3:1.3_3": "arahattamaggo arahattaphalaṁ,",
  "kv4.3:1.3_4": "satipaṭṭhānaṁ sammappadhānaṁ iddhipādo indriyaṁ balaṁ bojjhaṅgoti?",
  "kv4.3:1.4_0": "Na hevaṁ vattabbe …pe….",
  "kv4.3:2.1_0": "Arahato sabbe dhammā anāsavāti?",
  "kv4.3:2.2_0": "Āmantā.",
  "kv4.3:2.3_0": "Arahato cakkhuṁ anāsavanti?",
  "kv4.3:2.4_0": "Na hevaṁ vattabbe …pe….",
  "kv4.3:2.5_0": "Arahato cakkhuṁ anāsavanti?",
  "kv4.3:2.6_0": "Āmantā.",
  "kv4.3:2.7_0": "Maggo phalaṁ nibbānaṁ,",
  "kv4.3:2.7_1": "sotāpattimaggo sotāpattiphalaṁ …pe… bojjhaṅgoti?",
  "kv4.3:2.8_0": "Na hevaṁ vattabbe …pe….",
  "kv4.3:3.1_0": "Arahato sotaṁ …pe…",
  "kv4.3:3.2_0": "arahato ghānaṁ …",
  "kv4.3:3.3_0": "arahato jivhā …",
  "kv4.3:3.4_0": "arahato kāyo anāsavoti?",
  "kv4.3:3.5_0": "Na hevaṁ vattabbe …pe…",
  "kv4.3:3.6_0": "arahato kāyo anāsavoti?",
  "kv4.3:3.7_0": "Āmantā.",
  "kv4.3:3.8_0": "Maggo phalaṁ nibbānaṁ,",
  "kv4.3:3.8_1": "sotāpattimaggo sotāpattiphalaṁ …pe… bojjhaṅgoti?",
  "kv4.3:3.9_0": "Na hevaṁ vattabbe …pe….",
  "kv4.3:4.1_0": "Arahato kāyo anāsavoti?",
  "kv4.3:4.2_0": "Āmantā.",
  "kv4.3:4.3_0": "Arahato kāyo paggahaniggahupago chedanabhedanupago kākehi gijjhehi kulalehi sādhāraṇoti?",
  "kv4.3:4.4_0": "Āmantā.",
  "kv4.3:4.5_0": "Anāsavo dhammo paggahaniggahupago chedanabhedanupago kākehi gijjhehi kulalehi sādhāraṇoti?",
  "kv4.3:4.6_0": "Na hevaṁ vattabbe …pe….",
  "kv4.3:5.1_0": "Arahato kāye visaṁ kameyya, satthaṁ kameyya, aggi kameyyāti?",
  "kv4.3:5.2_0": "Āmantā.",
  "kv4.3:5.3_0": "Anāsave dhamme visaṁ kameyya, satthaṁ kameyya, aggi kameyyāti?",
  "kv4.3:5.4_0": "Na hevaṁ vattabbe …pe….",
  "kv4.3:6.1_0": "Labbhā arahato kāyo addubandhanena bandhituṁ, rajjubandhanena bandhituṁ,",
  "kv4.3:6.1_1": "saṅkhalikabandhanena bandhituṁ, gāmabandhanena bandhituṁ, nigamabandhanena bandhituṁ, nagarabandhanena bandhituṁ,",
  "kv4.3:6.1_2": "janapadabandhanena bandhituṁ,",
  "kv4.3:6.1_3": "kaṇṭhapañcamehi bandhanehi bandhitunti?",
  "kv4.3:6.2_0": "Āmantā.",
  "kv4.3:6.3_0": "Labbhā anāsavo dhammo addubandhanena bandhituṁ, rajjubandhanena bandhituṁ,",
  "kv4.3:6.3_1": "saṅkhalikabandhanena bandhituṁ,",
  "kv4.3:6.3_2": "gāmanigamanagarajanapadabandhanena bandhituṁ,",
  "kv4.3:6.3_3": "kaṇṭhapañcamehi bandhanehi bandhitunti?",
  "kv4.3:6.4_0": "Na hevaṁ vattabbe …pe….",
  "kv4.3:7.1_0": "Yadi arahā puthujjanassa cīvaraṁ deti,",
  "kv4.3:7.1_1": "anāsavaṁ hutvā sāsavaṁ hotīti?",
  "kv4.3:7.2_0": "Na hevaṁ vattabbe …pe…",
  "kv4.3:7.3_0": "anāsavaṁ hutvā sāsavaṁ hotīti?",
  "kv4.3:7.4_0": "Āmantā.",
  "kv4.3:7.5_0": "Taññeva anāsavaṁ taṁ sāsavanti?",
  "kv4.3:7.6_0": "Na hevaṁ vattabbe …pe…",
  "kv4.3:7.7_0": "taññeva anāsavaṁ taṁ sāsavanti?",
  "kv4.3:7.8_0": "Āmantā.",
