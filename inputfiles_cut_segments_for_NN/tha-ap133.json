{
  "tha-ap133:0.1_0": "Therāpadāna",
  "tha-ap133:0.2_0": "Sobhitavagga",
  "tha-ap133:0.3_0": "1. Sobhitattheraapadāna",
  "tha-ap133:1.1_0": "“Padumuttaro nāma jino,",
  "tha-ap133:1.2_0": "lokajeṭṭho narāsabho;",
  "tha-ap133:1.3_0": "Mahato janakāyassa,",
  "tha-ap133:1.4_0": "deseti amataṁ padaṁ.",
  "tha-ap133:2.1_0": "Tassāhaṁ vacanaṁ sutvā,",
  "tha-ap133:2.2_0": "vācāsabhimudīritaṁ;",
  "tha-ap133:2.3_0": "Añjaliṁ paggahetvāna,",
  "tha-ap133:2.4_0": "ekaggo āsahaṁ tadā.",
  "tha-ap133:3.1_0": "‘Yathā samuddo udadhīnamaggo,",
  "tha-ap133:3.2_0": "Nerū nagānaṁ pavaro siluccayo;",
  "tha-ap133:3.3_0": "Tatheva ye cittavasena vattare’,",
  "tha-ap133:3.4_0": "Na buddhañāṇassa kalaṁ upenti te.",
  "tha-ap133:4.1_0": "Dhammavidhiṁ ṭhapetvāna,",
  "tha-ap133:4.2_0": "buddho kāruṇiko isi;",
  "tha-ap133:4.3_0": "Bhikkhusaṅghe nisīditvā,",
  "tha-ap133:4.4_0": "imā gāthā abhāsatha.",
  "tha-ap133:5.1_0": "‘Yo so ñāṇaṁ pakittesi,",
  "tha-ap133:5.2_0": "buddhamhi lokanāyake;",
  "tha-ap133:5.3_0": "Kappānaṁ satasahassaṁ,",
  "tha-ap133:5.4_0": "duggatiṁ na gamissati.",
  "tha-ap133:6.1_0": "Kilese jhāpayitvāna,",
  "tha-ap133:6.2_0": "ekaggo susamāhito;",
  "tha-ap133:6.3_0": "Sobhito nāma nāmena,",
  "tha-ap133:6.4_0": "hessati satthu sāvako’.",
  "tha-ap133:7.1_0": "Paññāse kappasahasse,",
  "tha-ap133:7.2_0": "sattevāsuṁ yasuggatā;",
  "tha-ap133:7.3_0": "Sattaratanasampannā,",
  "tha-ap133:7.4_0": "cakkavattī mahabbalā.",
  "tha-ap133:8.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap133:8.2_0": "bhavā sabbe samūhatā;",
  "tha-ap133:8.3_0": "Tisso vijjā anuppattā,",
  "tha-ap133:8.4_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap133:9.1_0": "Paṭisambhidā catasso,",
  "tha-ap133:9.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap133:9.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap133:9.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap133:10.1_0": "Itthaṁ sudaṁ āyasmā sobhito thero imā gāthāyo abhāsitthāti.",
  "tha-ap133:11.1_0": "Sobhitattherassāpadānaṁ paṭhamaṁ."
}