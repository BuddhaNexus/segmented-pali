{
  "tha-ap82:0.1_0": "Therāpadāna",
  "tha-ap82:0.2_0": "Nāgasamālavagga",
  "tha-ap82:0.3_0": "10 Tipadumiyattheraapadāna",
  "tha-ap82:1.1_0": "“Padumuttaro nāma jino,",
  "tha-ap82:1.2_0": "sabbadhammāna pāragū;",
  "tha-ap82:1.3_0": "Danto dantaparivuto,",
  "tha-ap82:1.4_0": "nagarā nikkhamī tadā.",
  "tha-ap82:2.1_0": "Nagare haṁsavatiyaṁ,",
  "tha-ap82:2.2_0": "ahosiṁ māliko tadā;",
  "tha-ap82:2.3_0": "Yaṁ tattha uttamaṁ tīṇi,",
  "tha-ap82:2.4_0": "padmapupphāni aggahiṁ.",
  "tha-ap82:3.1_0": "‘Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap82:3.2_0": "paṭimaggantarāpaṇe;",
  "tha-ap82:3.3_0": "Saha disvāna sambuddhaṁ,",
  "tha-ap82:3.4_0": "evaṁ cintesahaṁ tadā.",
  "tha-ap82:4.1_0": "Kiṁ me imehi pupphehi,",
  "tha-ap82:4.2_0": "rañño upanitehi me;",
  "tha-ap82:4.3_0": "Gāmaṁ vā gāmakhettaṁ vā,",
  "tha-ap82:4.4_0": "sahassaṁ vā labheyyahaṁ.",
  "tha-ap82:5.1_0": "Adantadamanaṁ vīraṁ,",
  "tha-ap82:5.2_0": "sabbasattasukhāvahaṁ;",
  "tha-ap82:5.3_0": "Lokanāthaṁ pūjayitvā,",
  "tha-ap82:5.4_0": "lacchāmi amataṁ dhanaṁ’.",
  "tha-ap82:6.1_0": "Evāhaṁ cintayitvāna,",
  "tha-ap82:6.2_0": "sakaṁ cittaṁ pasādayiṁ;",
  "tha-ap82:6.3_0": "Tīṇi lohitake gayha,",
  "tha-ap82:6.4_0": "ākāse ukkhipiṁ tadā.",
  "tha-ap82:7.1_0": "Mayā ukkhittamattamhi,",
  "tha-ap82:7.2_0": "ākāse patthariṁsu te;",
  "tha-ap82:7.3_0": "Dhāriṁsu matthake tattha,",
  "tha-ap82:7.4_0": "uddhaṁvaṇṭā adhomukhā.",
  "tha-ap82:8.1_0": "Ye keci manujā disvā,",
  "tha-ap82:8.2_0": "ukkuṭṭhiṁ sampavattayuṁ;",
  "tha-ap82:8.3_0": "Devatā antalikkhamhi,",
  "tha-ap82:8.4_0": "sādhukāraṁ pavattayuṁ.",
  "tha-ap82:9.1_0": "Accheraṁ loke uppannaṁ,",
  "tha-ap82:9.2_0": "buddhaseṭṭhassa vāhasā;",
  "tha-ap82:9.3_0": "Sabbe dhammaṁ suṇissāma,",
  "tha-ap82:9.4_0": "pupphānaṁ vāhasā mayaṁ.",
  "tha-ap82:10.1_0": "Padumuttaro lokavidū,",
  "tha-ap82:10.2_0": "āhutīnaṁ paṭiggaho;",
  "tha-ap82:10.3_0": "Vīthiyañhi ṭhito santo,",
  "tha-ap82:10.4_0": "imā gāthā abhāsatha.",
  "tha-ap82:11.1_0": "‘Yo so buddhaṁ apūjesi,",
  "tha-ap82:11.2_0": "rattapadmehi māṇavo;",
  "tha-ap82:11.3_0": "Tamahaṁ kittayissāmi,",
  "tha-ap82:11.4_0": "suṇātha mama bhāsato.",
  "tha-ap82:12.1_0": "Tiṁsakappasahassāni,",
  "tha-ap82:12.2_0": "devaloke ramissati;",
  "tha-ap82:12.3_0": "Tiṁsakappāni devindo,",
  "tha-ap82:12.4_0": "devarajjaṁ karissati.",
  "tha-ap82:13.1_0": "Mahāvitthārikaṁ nāma,",
  "tha-ap82:13.2_0": "byamhaṁ hessati tāvade;",
  "tha-ap82:13.3_0": "Tiyojanasatubbiddhaṁ,",
  "tha-ap82:13.4_0": "diyaḍḍhasatavitthataṁ.",
  "tha-ap82:14.1_0": "Cattārisatasahassāni,",
  "tha-ap82:14.2_0": "niyyūhā ca sumāpitā;",
  "tha-ap82:14.3_0": "Kūṭāgāravarūpetā,",
  "tha-ap82:14.4_0": "mahāsayanamaṇḍitā.",
  "tha-ap82:15.1_0": "Koṭisatasahassiyo,",
  "tha-ap82:15.2_0": "parivāressanti accharā;",
  "tha-ap82:15.3_0": "Kusalā naccagītassa,",
  "tha-ap82:15.4_0": "vāditepi padakkhiṇā.",
  "tha-ap82:16.1_0": "Etādise byamhavare,",
  "tha-ap82:16.2_0": "nārīgaṇasamākule;",
  "tha-ap82:16.3_0": "Vassissati pupphavasso,",
  "tha-ap82:16.4_0": "dibbo lohitako sadā.",
  "tha-ap82:17.1_0": "Bhittikhīle nāgadante,",
  "tha-ap82:17.2_0": "dvārabāhāya toraṇe;",
  "tha-ap82:17.3_0": "Cakkamattā lohitakā,",
  "tha-ap82:17.4_0": "olambissanti tāvade.",
  "tha-ap82:18.1_0": "Pattena pattasañchanne,",
  "tha-ap82:18.2_0": "antobyamhavare imaṁ;",
  "tha-ap82:18.3_0": "Attharitvā pārupitvā,",
  "tha-ap82:18.4_0": "tuvaṭṭissanti tāvade.",
  "tha-ap82:19.1_0": "Bhavanaṁ parivāretvā,",
  "tha-ap82:19.2_0": "samantā satayojane;",
  "tha-ap82:19.3_0": "Tepi padmā lohitakā,",
  "tha-ap82:19.4_0": "dibbagandhaṁ pavāyare.",
  "tha-ap82:20.1_0": "Pañcasattatikkhattuñca,",
  "tha-ap82:20.2_0": "cakkavattī bhavissati;",
  "tha-ap82:20.3_0": "Padesarajjaṁ vipulaṁ,",
  "tha-ap82:20.4_0": "gaṇanāto asaṅkhiyaṁ.",
  "tha-ap82:21.1_0": "Sampattiyo duve bhutvā,",
  "tha-ap82:21.2_0": "anīti anupaddavo;",
  "tha-ap82:21.3_0": "Sampatte pariyosāne,",
  "tha-ap82:21.4_0": "nibbānaṁ pāpuṇissati’.",
  "tha-ap82:22.1_0": "Sudiṭṭho vata me buddho,",
  "tha-ap82:22.2_0": "vāṇijjaṁ supayojitaṁ;",
  "tha-ap82:22.3_0": "Padmāni tīṇi pūjetvā,",
  "tha-ap82:22.4_0": "anubhosiṁ tisampadā.",
  "tha-ap82:23.1_0": "Ajja me dhammappattassa,",
  "tha-ap82:23.2_0": "vippamuttassa sabbaso;",
  "tha-ap82:23.3_0": "Supupphitaṁ lohitakaṁ,",
  "tha-ap82:23.4_0": "dhārayissati matthake.",
  "tha-ap82:24.1_0": "Mama kammaṁ kathentassa,",
  "tha-ap82:24.2_0": "padumuttarasatthuno;",
  "tha-ap82:24.3_0": "Satapāṇasahassānaṁ,",
  "tha-ap82:24.4_0": "dhammābhisamayo ahu.",
  "tha-ap82:25.1_0": "Satasahassito kappe,",
  "tha-ap82:25.2_0": "yaṁ buddhamabhipūjayiṁ;",
  "tha-ap82:25.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap82:25.4_0": "tipadumānidaṁ phalaṁ.",
  "tha-ap82:26.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap82:26.2_0": "bhavā sabbe samūhatā;",
  "tha-ap82:26.3_0": "Sabbāsavā parikkhīṇā,",
  "tha-ap82:26.4_0": "natthi dāni punabbhavo.",
  "tha-ap82:27.1_0": "Paṭisambhidā catasso,",
  "tha-ap82:27.2_0": "…pe…",
  "tha-ap82:27.3_0": "kataṁ buddhassa sāsanaṁ”. ",
  "tha-ap82:28.1_0": "Itthaṁ sudaṁ āyasmā tipadumiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap82:29.1_0": "Tipadumiyattherassāpadānaṁ dasamaṁ.",
  "tha-ap82:30.1_0": "Nāgasamālavaggo aṭṭhamo.",
  "tha-ap82:31.1_0": "Tassuddānaṁ",
  "tha-ap82:32.1_0": "Nāgasamālo padasaññī,",
  "tha-ap82:32.2_0": "saññakāluvadāyako;",
  "tha-ap82:32.3_0": "Ekasaññī tiṇasanthāro,",
  "tha-ap82:32.4_0": "sūcipāṭalipupphiyo;",
  "tha-ap82:32.5_0": "Ṭhitañjalī tipadumī,",
  "tha-ap82:32.6_0": "gāthāyo pañcasattati."
}