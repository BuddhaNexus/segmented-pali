{
  "mil4.6:0.1_0": "Milindapañha",
  "mil4.6:0.2_0": "Meṇḍakapañhārambhakathā",
  "mil4.6:0.3_0": "Upāsakaguṇavagga",
  "mil4.6:0.4_0": "6. Upāsakaguṇa",
  "mil4.6:1.1_0": "Thero “sādhū”ti sampaṭicchitvā dasa upāsakassa upāsakaguṇe paridīpesi.",
  "mil4.6:1.2_0": "“Dasa ime, mahārāja, upāsakassa upāsakaguṇā.",
  "mil4.6:1.3_0": "Katame dasa, idha, mahārāja,",
  "mil4.6:1.3_1": "upāsako saṅghena samānasukhadukkho hoti,",
  "mil4.6:1.3_2": "dhammādhipateyyo hoti,",
  "mil4.6:1.3_3": "yathābalaṁ saṁvibhāgarato hoti,",
  "mil4.6:1.3_4": "jinasāsanaparihāniṁ disvā abhivaḍḍhiyā vāyamati.",
  "mil4.6:1.4_0": "Sammādiṭṭhiko hoti,",
  "mil4.6:1.4_1": "apagatakotūhalamaṅgaliko jīvitahetupi na aññaṁ satthāraṁ uddisati,",
  "mil4.6:1.4_2": "kāyikavācasikañcassa rakkhitaṁ hoti,",
  "mil4.6:1.4_3": "samaggārāmo hoti samaggarato,",
  "mil4.6:1.4_4": "anusūyako hoti,",
  "mil4.6:1.4_5": "na ca kuhanavasena sāsane carati,",
  "mil4.6:1.4_6": "buddhaṁ saraṇaṁ gato hoti,",
  "mil4.6:1.4_7": "dhammaṁ saraṇaṁ gato hoti,",
  "mil4.6:1.4_8": "saṅghaṁ saraṇaṁ gato hoti.",
  "mil4.6:1.5_0": "Ime kho, mahārāja,",
  "mil4.6:1.5_1": "dasa upāsakassa upāsakaguṇā,",
  "mil4.6:1.5_2": "te sabbe guṇā tayi saṁvijjanti,",
  "mil4.6:1.5_3": "taṁ te yuttaṁ pattaṁ anucchavikaṁ patirūpaṁ yaṁ tvaṁ jinasāsanaparihāniṁ disvā abhivaḍḍhiṁ icchasi,",
  "mil4.6:1.5_4": "karomi te okāsaṁ,",
  "mil4.6:1.5_5": "puccha maṁ tvaṁ yathāsukhan”ti.",
  "mil4.6:2.1_0": "Meṇḍakapañhārambhakathā niṭṭhitā."
}