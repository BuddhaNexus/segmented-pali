{
  "anya-e1009n10:0_0": "Kāyakhamanīya-niddesa",
  "anya-e1009n10:1_0": "234.",
  "anya-e1009n10:2_0": " Abhivādana-sīlissa ,",
  "anya-e1009n10:3_0": "Niccaṁ vuḍḍhāpacāyino;",
  "anya-e1009n10:4_0": "Cattāro dhammā vaḍḍhanti,",
  "anya-e1009n10:5_0": "Āyu vaṇṇo sukhaṁ balaṁ.",
  "anya-e1009n10:6_0": "235.",
  "anya-e1009n10:6_1": "Pañcime bhikkhave dhammā āyussā,",
  "anya-e1009n10:6_2": "katame pañca.",
  "anya-e1009n10:6_3": "Sappāya-kārī hoti.",
  "anya-e1009n10:6_4": "Sappāyeca mattaṁ jānāti.",
  "anya-e1009n10:6_5": "Pariṇattabhojī ca hoti.",
  "anya-e1009n10:6_6": "Kāla-cārī ca,",
  "anya-e1009n10:6_7": "brahma-cārīca.",
  "anya-e1009n10:6_8": "Ime kho bhikkhave pañca dhammā āyussāti.",
  "anya-e1009n10:7_0": "236.",
  "anya-e1009n10:7_1": "Pañcime bhikkhave dhammā āyussā,",
  "anya-e1009n10:7_2": "katame pañca.",
  "anya-e1009n10:7_3": "Sappāya-kārī hoti.",
  "anya-e1009n10:7_4": "Sappāyeca mattaṁ jānāti.",
  "anya-e1009n10:7_5": "Pariṇatabhojīcahoti. Sīlavāca,",
  "anya-e1009n10:7_6": "kalyāṇa mittoca.",
  "anya-e1009n10:7_7": "Ime kho bhikkhave pañca dhammā āyussāti.",
  "anya-e1009n10:8_0": "237.",
  "anya-e1009n10:9_0": " Pañca-sīlaṁ samādāya,",
  "anya-e1009n10:10_0": "Samaṁ katvā dine dine;",
  "anya-e1009n10:11_0": "Satimā paññavā hutvā,",
  "anya-e1009n10:12_0": "Care sabbiriyāpathe.",
  "anya-e1009n10:13_0": "238.",
  "anya-e1009n10:13_1": "Pañcime bhikkhave caṅkame ānisaṁsā,",
  "anya-e1009n10:13_2": "katame pañca,",
  "anya-e1009n10:13_3": "addhānakkhamo hoti.",
  "anya-e1009n10:13_4": "Padhānakkhamo hoti.",
  "anya-e1009n10:13_5": "Appābādho hoti.",
  "anya-e1009n10:13_6": "Asitaṁpītaṁkhāyitaṁsāyitaṁ sammā pariṇāmaṁ gacchati.",
  "anya-e1009n10:13_7": "Caṅkamādhigato samādhi ciraṭṭhitiko hoti.",
  "anya-e1009n10:13_8": "Imekho bhikkhave pañca caṅkame āni saṁsāti.",
  "anya-e1009n10:14_0": "239.",
  "anya-e1009n10:15_0": " Parissāvana-dānañca ,",
  "anya-e1009n10:16_0": "Āvāsa-dānameva ca;",
  "anya-e1009n10:17_0": "Gilāna-vatthu-dānañca,",
  "anya-e1009n10:18_0": "Dātabbaṁ manujādhipa.",
  "anya-e1009n10:19_0": "240.",
  "anya-e1009n10:20_0": " Kātabbaṁ jiṇṇakāvāsaṁ,",
  "anya-e1009n10:21_0": "Paṭisaṅkharaṇaṁ tathā;",
  "anya-e1009n10:22_0": "Pañca-sīla-samādānaṁ,",
  "anya-e1009n10:23_0": "Katvā taṁ sādhu-rakkhitaṁ;",
  "anya-e1009n10:24_0": "Uposathopavāso ca,",
  "anya-e1009n10:25_0": "Kātabboposathe iti.",
  "anya-e1009n10:26_0": "241.",
  "anya-e1009n10:27_0": " Ati-bhottā roga-mūlaṁ,",
  "anya-e1009n10:28_0": "Āyukkhayaṁ karoti ve;",
  "anya-e1009n10:29_0": "Tasmā taṁ ati-bhuttiṁva,",
  "anya-e1009n10:30_0": "Parihareyya paṇḍito.",
  "anya-e1009n10:31_0": "242.",
  "anya-e1009n10:32_0": " A-jiṇṇe bhojanaṁ visaṁ,",
  "anya-e1009n10:33_0": "Dulladdhe a-vicārake;",
  "anya-e1009n10:34_0": "Jiṇṇe su-laddhe vicāre,",
  "anya-e1009n10:35_0": "Na vajjaṁ sabba-bhojanaṁ.",
  "anya-e1009n10:36_0": "243.",
  "anya-e1009n10:37_0": " Cattāro pañca ālope,",
  "anya-e1009n10:38_0": "Ābhutvā udakaṁ pive;",
  "anya-e1009n10:39_0": "Alaṁ phāsu-vihārāya;",
  "anya-e1009n10:40_0": "Pahitattassa bhikkhuno.",
  "anya-e1009n10:41_0": "244.",
  "anya-e1009n10:42_0": " Manujassa sadā satimato,",
  "anya-e1009n10:43_0": "Mattaṁ jānato laddha-bhojanaṁ;",
  "anya-e1009n10:44_0": "Tanukassa bhavanti vedanā,",
  "anya-e1009n10:45_0": "Sanikaṁ jīrati āyu pālayaṁ.",
  "anya-e1009n10:46_0": "245.",
  "anya-e1009n10:47_0": " Garūnaṁ aḍḍha-sohiccaṁ,",
  "anya-e1009n10:48_0": "Lahūnaṁ nāti-kittiyā;",
  "anya-e1009n10:49_0": "Mattā-pamāṇaṁ niddiṭṭhaṁ,",
  "anya-e1009n10:50_0": "Sukhaṁ jīrati tāvatā.",
  "anya-e1009n10:51_0": "246.",
  "anya-e1009n10:52_0": " Toyābhāve pipāsattā,",
  "anya-e1009n10:53_0": "Khaṇā pāṇehi muccate;",
  "anya-e1009n10:54_0": "Tasmā sabbāsuvatthāsu,",
  "anya-e1009n10:55_0": "Deyyaṁ vāriṁ pipāsaye.",
  "anya-e1009n10:56_0": "247.",
  "anya-e1009n10:57_0": " Sītodakaṁ payo khuddaṁ,",
  "anya-e1009n10:58_0": "Ghatamekekaso dviso;",
  "anya-e1009n10:59_0": "Tisso samaggamatha vā,",
  "anya-e1009n10:60_0": "Page pitaṁ yuvattadaṁ.",
  "anya-e1009n10:61_0": "248.",
  "anya-e1009n10:62_0": " Annaṁ brahmā rase viṇhu,",
  "anya-e1009n10:63_0": "Bhutte ceva mahesaro;",
  "anya-e1009n10:64_0": "Evaṁ ñatvātu yo bhuñje,",
  "anya-e1009n10:65_0": "Anna-dosaṁ na limpate.",
  "anya-e1009n10:66_0": "249.",
  "anya-e1009n10:67_0": " Kattikassantimo bhāgo,",
  "anya-e1009n10:68_0": "Yaṁ cādo miga-māsajo;",
  "anya-e1009n10:69_0": "Tāvubho yama-dāṭhākhyo,",
  "anya-e1009n10:70_0": "Laghvāhārova jīvati.",
  "anya-e1009n10:71_0": "250.",
  "anya-e1009n10:72_0": " Satthānukula-cariyā ,",
  "anya-e1009n10:73_0": "Cittaññāvasavattinā;",
  "anya-e1009n10:74_0": "Buddhi-rakkhilitatthena,",
  "anya-e1009n10:75_0": "Paripuṇṇaṁ rasāyanaṁ.",
  "anya-e1009n10:76_0": "250.",
  "anya-e1009n10:77_0": " A-jātiyā a-jātānaṁ,",
  "anya-e1009n10:78_0": "Jātānaṁ vinivattiyā;",
  "anya-e1009n10:79_0": "Rogānaṁ yo vidhi diṭṭho,",
  "anya-e1009n10:80_0": "Taṁ sukhatthī samācare.",
  "anya-e1009n10:81_0": "252.",
  "anya-e1009n10:82_0": " Ārogyaṁ paramā lābhā,",
  "anya-e1009n10:83_0": "Santuṭṭhi paramaṁ dhanaṁ;",
  "anya-e1009n10:84_0": "Vissāsā paramā ñāti,",
  "anya-e1009n10:85_0": "Nibbānaṁ paramaṁ sukhaṁ."
}