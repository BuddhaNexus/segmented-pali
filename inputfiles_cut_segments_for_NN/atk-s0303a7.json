{
  "atk-s0303a7:0_0": "8. Nāgasaṁyuttaṁ",
  "atk-s0303a7:1_0": "1. Suddhikasuttavaṇṇanā",
  "atk-s0303a7:2_0": "342.",
  "atk-s0303a7:2_1": "Nāgasaṁyutte aṇḍajāti aṇḍe jātā.",
  "atk-s0303a7:2_2": "Jalābujāti vatthikose jātā.",
  "atk-s0303a7:2_3": "Saṁsedajāti saṁsede jātā.",
  "atk-s0303a7:2_4": "Opapātikāti upapatitvā viya jātā.",
  "atk-s0303a7:2_5": "Idañca pana suttaṁ aṭṭhuppattiyā vuttaṁ.",
  "atk-s0303a7:2_6": "Bhikkhūnañhi ‘‘kati nu kho nāgayoniyo’’ti kathā udapādi.",
  "atk-s0303a7:2_7": "Atha bhagavā puggalānaṁ nāgayonīhi uddharaṇatthaṁ nāgayoniyo āvikaronto imaṁ suttamāha.",
  "atk-s0303a7:3_0": "2-50. Paṇītatarasuttādivaṇṇanā",
  "atk-s0303a7:4_0": "343-391.",
  "atk-s0303a7:4_1": "Dutiyādīsu vossaṭṭhakāyāti ahituṇḍikaparibuddhaṁ agaṇetvā vissaṭṭhakāyā.",
  "atk-s0303a7:4_2": "Dvayakārinoti duvidhakārino,",
  "atk-s0303a7:4_3": "kusalākusalakārinoti attho.",
  "atk-s0303a7:4_4": "Sacajja mayanti sace ajja mayaṁ.",
  "atk-s0303a7:4_5": "Sahabyataṁ upapajjatīti sahabhāvaṁ āpajjati.",
  "atk-s0303a7:4_6": "Tatrassa akusalaṁ upapattiyā paccayo hoti,",
  "atk-s0303a7:4_7": "kusalaṁ upapannānaṁ sampattiyā.",
  "atk-s0303a7:4_8": "Annanti khādanīyabhojanīyaṁ.",
  "atk-s0303a7:4_9": "Pānanti yaṁkiñci pānakaṁ.",
  "atk-s0303a7:4_10": "Vatthanti nivāsanapārupanaṁ.",
  "atk-s0303a7:4_11": "Yānanti chattupāhanaṁ ādiṁ katvā yaṁkiñci gamanapaccayaṁ.",
  "atk-s0303a7:4_12": "Mālanti yaṁkiñci sumanamālādipupphaṁ.",
  "atk-s0303a7:4_13": "Gandhanti yaṁkiñci candanādigandhaṁ.",
  "atk-s0303a7:4_14": "Vilepananti yaṁkiñci chavirāgakaraṇaṁ.",
  "atk-s0303a7:4_15": "Seyyāvasathapadīpeyyanti mañcapīṭhādiseyyaṁ ekabhūmikādiāvasathaṁ vaṭṭitelādipadīpūpakaraṇañca detīti attho.",
  "atk-s0303a7:4_16": "Tesañhi dīghāyukatāya ca vaṇṇavantatāya ca sukhabahulatāya ca patthanaṁ katvā imaṁ dasavidhaṁ dānavatthuṁ",
  "atk-s0303a7:4_17": "datvā taṁ sampattiṁ anubhavituṁ tattha nibbattanti.",
  "atk-s0303a7:4_18": "Sesaṁ sabbattha uttānamevāti.",
  "atk-s0303a7:5_0": "Nāgasaṁyuttavaṇṇanā niṭṭhitā."
}