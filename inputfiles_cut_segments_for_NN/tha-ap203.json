{
  "tha-ap203:0.1_0": "Therāpadāna",
  "tha-ap203:0.2_0": "Kaṇikārapupphiyavagga",
  "tha-ap203:0.3_0": "1. Kaṇikārapupphiyattheraapadāna",
  "tha-ap203:1.1_0": "“Kaṇikāraṁ pupphitaṁ disvā,",
  "tha-ap203:1.2_0": "ocinitvānahaṁ tadā;",
  "tha-ap203:1.3_0": "Tissassa abhiropesiṁ,",
  "tha-ap203:1.4_0": "oghatiṇṇassa tādino.",
  "tha-ap203:2.1_0": "Dvenavute ito kappe,",
  "tha-ap203:2.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap203:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap203:2.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap203:3.1_0": "Pañcattiṁse ito kappe,",
  "tha-ap203:3.2_0": "aruṇapāṇīti vissuto;",
  "tha-ap203:3.3_0": "Sattaratanasampanno,",
  "tha-ap203:3.4_0": "cakkavattī mahabbalo.",
  "tha-ap203:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap203:4.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap203:4.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap203:4.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap203:5.1_0": "Itthaṁ sudaṁ āyasmā kaṇikārapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap203:6.1_0": "Kaṇikārapupphiyattherassāpadānaṁ paṭhamaṁ."
}