{
  "ja121:0.1_0": "Jātaka",
  "ja121:0.2_0": "Ekakanipāta",
  "ja121:0.3_0": "Kusanāḷivagga",
  "ja121:0.4_0": "1. Kusanāḷijātaka",
  "ja121:1.1_0": "“Kare sarikkho atha vāpi seṭṭho,",
  "ja121:1.2_0": "Nihīnako vāpi kareyya eko;",
  "ja121:1.3_0": "Kareyyumete byasane uttamatthaṁ,",
  "ja121:1.4_0": "Yathā ahaṁ kusanāḷi rucāyan”ti.",
  "ja121:2.1_0": "Kusanāḷijātakaṁ paṭhamaṁ."
}