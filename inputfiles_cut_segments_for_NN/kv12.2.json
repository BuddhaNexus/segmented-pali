{
  "kv12.2:0.1_0": "Kathāvatthu",
  "kv12.2:0.2_0": "Mahāpaṇṇāsaka",
  "kv12.2:0.3_0": "Dvādasamavagga",
  "kv12.2:0.4_0": "Kammakathā",
  "kv12.2:1.1_0": "Sabbaṁ kammaṁ savipākanti?",
  "kv12.2:1.2_0": "Āmantā.",
  "kv12.2:1.3_0": "Sabbā cetanā savipākāti?",
  "kv12.2:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv12.2:1.5_0": "sabbā cetanā savipākāti?",
  "kv12.2:1.6_0": "Āmantā.",
  "kv12.2:1.7_0": "Vipākābyākatā cetanā savipākāti?",
  "kv12.2:1.8_0": "Na hevaṁ vattabbe …pe…",
  "kv12.2:1.9_0": "sabbā cetanā savipākāti?",
  "kv12.2:1.10_0": "Āmantā.",
  "kv12.2:1.11_0": "Kiriyābyākatā cetanā savipākāti?",
  "kv12.2:1.12_0": "Na hevaṁ vattabbe …pe….",
  "kv12.2:2.1_0": "Sabbā cetanā savipākāti?",
  "kv12.2:2.2_0": "Āmantā.",
  "kv12.2:2.3_0": "Kāmāvacarā vipākābyākatā cetanā savipākāti?",
  "kv12.2:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv12.2:2.5_0": "sabbā cetanā savipākāti?",
  "kv12.2:2.6_0": "Āmantā.",
  "kv12.2:2.7_0": "Rūpāvacarā arūpāvacarā apariyāpannā vipākābyākatā cetanā savipākāti?",
  "kv12.2:2.8_0": "Na hevaṁ vattabbe …pe….",
  "kv12.2:3.1_0": "Sabbā cetanā savipākāti?",
  "kv12.2:3.2_0": "Āmantā.",
  "kv12.2:3.3_0": "Kāmāvacarā kiriyābyākatā cetanā savipākāti?",
  "kv12.2:3.4_0": "Na hevaṁ vattabbe …pe…",
  "kv12.2:3.5_0": "sabbā cetanā savipākāti?",
  "kv12.2:3.6_0": "Āmantā.",
  "kv12.2:3.7_0": "Rūpāvacarā arūpāvacarā kiriyābyākatā cetanā savipākāti?",
  "kv12.2:3.8_0": "Na hevaṁ vattabbe …pe….",
  "kv12.2:4.1_0": "Vipākābyākatā cetanā avipākāti?",
  "kv12.2:4.2_0": "Āmantā.",
  "kv12.2:4.3_0": "Hañci vipākābyākatā cetanā avipākā, no ca vata re vattabbe—",
  "kv12.2:4.4_0": "“sabbā cetanā savipākā”ti.",
  "kv12.2:5.1_0": "Kiriyābyākatā cetanā avipākāti?",
  "kv12.2:5.2_0": "Āmantā.",
  "kv12.2:5.3_0": "Hañci kiriyābyākatā cetanā avipākā, no ca vata re vattabbe—",
  "kv12.2:5.4_0": "“sabbā cetanā savipākā”ti.",
  "kv12.2:6.1_0": "Kāmāvacarā rūpāvacarā arūpāvacarā apariyāpannā vipākābyākatā cetanā avipākāti?",
  "kv12.2:6.2_0": "Āmantā.",
  "kv12.2:6.3_0": "Hañci apariyāpannā vipākābyākatā cetanā avipākā, no ca vata re vattabbe—",
  "kv12.2:6.4_0": "“sabbā cetanā savipākā”ti.",
  "kv12.2:7.1_0": "Kāmāvacarā rūpāvacarā arūpāvacarā kiriyābyākatā cetanā avipākāti?",
  "kv12.2:7.2_0": "Āmantā.",
  "kv12.2:7.3_0": "Hañci arūpāvacarā kiriyābyākatā cetanā avipākā, no ca vata re vattabbe—",
  "kv12.2:7.4_0": "“sabbā cetanā savipākā”ti.",
  "kv12.2:8.1_0": "Na vattabbaṁ—",
  "kv12.2:8.2_0": "“sabbaṁ kammaṁ savipākan”ti?",
  "kv12.2:8.3_0": "Āmantā.",
  "kv12.2:8.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv12.2:8.5_0": "“nāhaṁ,",
  "kv12.2:8.5_1": "bhikkhave,",
  "kv12.2:8.5_2": "sañcetanikānaṁ kammānaṁ katānaṁ upacitānaṁ appaṭisaṁviditvā byantibhāvaṁ vadāmi,",
  "kv12.2:8.5_3": "tañca kho diṭṭheva dhamme upapajje vā apare vā pariyāye”ti.",
  "kv12.2:8.6_0": "Attheva suttantoti?",
  "kv12.2:8.7_0": "Āmantā.",
  "kv12.2:8.8_0": "Tena hi sabbaṁ kammaṁ savipākanti.",
  "kv12.2:8.9_0": "Kammakathā niṭṭhitā."
}