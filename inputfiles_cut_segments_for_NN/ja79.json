{
  "ja79:0.1_0": "Jātaka",
  "ja79:0.2_0": "Ekakanipāta",
  "ja79:0.3_0": "Varuṇavagga",
  "ja79:0.4_0": "9. Kharassarajātaka",
  "ja79:1.1_0": "“Yato viluttā ca hatā ca gāvo,",
  "ja79:1.2_0": "Daḍḍhāni gehāni jano ca nīto;",
  "ja79:1.3_0": "Athāgamā puttahatāya putto,",
  "ja79:1.4_0": "Kharassaraṁ ḍiṇḍimaṁ vādayanto”ti.",
  "ja79:2.1_0": "Kharassarajātakaṁ navamaṁ."
}