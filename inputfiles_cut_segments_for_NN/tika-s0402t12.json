{
  "tika-s0402t12:0_0": "(13) 3. Dānavaggavaṇṇanā",
  "tika-s0402t12:1_0": "142.",
  "tika-s0402t12:1_1": "Tatiyassa paṭhame dīyatīti dānaṁ,",
  "tika-s0402t12:1_2": "deyyadhammassetaṁ adhivacanaṁ.",
  "tika-s0402t12:1_3": "Dīyati anenāti vā dānaṁ,",
  "tika-s0402t12:1_4": "pariccāgacetanāyetaṁ adhivacanaṁ.",
  "tika-s0402t12:1_5": "Ayaṁ duvidhopi attho idhādhippetoti āha ‘‘diyyanakavasena dānānī’’tiādi.",
  "tika-s0402t12:1_6": "Tattha diyyanakavasenāti dātabbavasena.",
  "tika-s0402t12:1_7": "Amatapattipaṭipadanti amatappattihetubhūtaṁ sammāpaṭipadaṁ.",
  "tika-s0402t12:2_0": "143-151. Dutiyādīni ca suviññeyyāneva.",
  "tika-s0402t12:3_0": "Dānavaggavaṇṇanā niṭṭhitā."
}