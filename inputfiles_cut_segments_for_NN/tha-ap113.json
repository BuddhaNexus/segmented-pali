{
  "tha-ap113:0.1_0": "Therāpadāna",
  "tha-ap113:0.2_0": "Mahāparivāravagga",
  "tha-ap113:0.3_0": "1 Mahāparivārakattheraapadāna",
  "tha-ap113:1.1_0": "“Vipassī nāma bhagavā,",
  "tha-ap113:1.2_0": "lokajeṭṭho narāsabho;",
  "tha-ap113:1.3_0": "Aṭṭhasaṭṭhisahassehi,",
  "tha-ap113:1.4_0": "pāvisi bandhumaṁ tadā.",
  "tha-ap113:2.1_0": "Nagarā abhinikkhamma,",
  "tha-ap113:2.2_0": "agamaṁ dīpacetiyaṁ;",
  "tha-ap113:2.3_0": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap113:2.4_0": "āhutīnaṁ paṭiggahaṁ.",
  "tha-ap113:3.1_0": "Cullāsītisahassāni,",
  "tha-ap113:3.2_0": "yakkhā mayhaṁ upantike;",
  "tha-ap113:3.3_0": "Upaṭṭhahanti sakkaccaṁ,",
  "tha-ap113:3.4_0": "indaṁva tidasā gaṇā.",
  "tha-ap113:4.1_0": "Bhavanā abhinikkhamma,",
  "tha-ap113:4.2_0": "dussaṁ paggayhahaṁ tadā;",
  "tha-ap113:4.3_0": "Sirasā abhivādesiṁ,",
  "tha-ap113:4.4_0": "tañcādāsiṁ mahesino.",
  "tha-ap113:5.1_0": "Aho buddhā aho dhammā,",
  "tha-ap113:5.2_0": "aho no satthu sampadā;",
  "tha-ap113:5.3_0": "Buddhassa ānubhāvena,",
  "tha-ap113:5.4_0": "vasudhāyaṁ pakampatha.",
  "tha-ap113:6.1_0": "Tañca acchariyaṁ disvā,",
  "tha-ap113:6.2_0": "abbhutaṁ lomahaṁsanaṁ;",
  "tha-ap113:6.3_0": "Buddhe cittaṁ pasādemi,",
  "tha-ap113:6.4_0": "dvipadindamhi tādine.",
  "tha-ap113:7.1_0": "Sohaṁ cittaṁ pasādetvā,",
  "tha-ap113:7.2_0": "dussaṁ datvāna satthuno;",
  "tha-ap113:7.3_0": "Saraṇañca upāgacchiṁ,",
  "tha-ap113:7.4_0": "sāmacco saparijjano.",
  "tha-ap113:8.1_0": "Ekanavutito kappe,",
  "tha-ap113:8.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap113:8.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap113:8.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap113:9.1_0": "Ito pannarase kappe,",
  "tha-ap113:9.2_0": "soḷasāsuṁ suvāhanā;",
  "tha-ap113:9.3_0": "Sattaratanasampannā,",
  "tha-ap113:9.4_0": "cakkavattī mahabbalā.",
  "tha-ap113:10.1_0": "Paṭisambhidā catasso,",
  "tha-ap113:10.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap113:10.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap113:10.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap113:11.1_0": "Itthaṁ sudaṁ āyasmā mahāparivārako thero imā gāthāyo abhāsitthāti.",
  "tha-ap113:12.1_0": "Mahāparivārakattherassāpadānaṁ paṭhamaṁ."
}