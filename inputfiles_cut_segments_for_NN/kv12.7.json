{
  "kv12.7:0.1_0": "Kathāvatthu",
  "kv12.7:0.2_0": "Mahāpaṇṇāsaka",
  "kv12.7:0.3_0": "Dvādasamavagga",
  "kv12.7:0.4_0": "Ekabījīkathā",
  "kv12.7:1.1_0": "Na vattabbaṁ—",
  "kv12.7:1.2_0": "“ekabījī puggalo ekabījitāniyato”ti?",
  "kv12.7:1.3_0": "Āmantā.",
  "kv12.7:1.4_0": "Nanu so ekabījīti?",
  "kv12.7:1.5_0": "Āmantā.",
  "kv12.7:1.6_0": "Hañci so ekabījī, tena vata re vattabbe—",
  "kv12.7:1.7_0": "“ekabījī puggalo ekabījitāniyato”ti.",
  "kv12.7:1.8_0": "Ekabījīkathā niṭṭhitā."
}