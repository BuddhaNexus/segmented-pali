{
  "ja464:0.1_0": "Jātaka",
  "ja464:0.2_0": "Dvādasakanipāta",
  "ja464:0.3_0": "Cūḷakuṇālavagga",
  "ja464:0.4_0": "1. Cūḷakuṇālajātaka",
  "ja464:1.1_0": "“Luddhānaṁ lahucittānaṁ,",
  "ja464:1.2_0": "akataññūna dubbhinaṁ;",
  "ja464:1.3_0": "Nādevasatto puriso,",
  "ja464:1.4_0": "thīnaṁ saddhātumarahati.",
  "ja464:2.1_0": "Na tā pajānanti kataṁ na kiccaṁ,",
  "ja464:2.2_0": "Na mātaraṁ pitaraṁ bhātaraṁ vā;",
  "ja464:2.3_0": "Anariyā samatikkantadhammā,",
  "ja464:2.4_0": "Sasseva cittassa vasaṁ vajanti.",
  "ja464:3.1_0": "Cirānuvutthampi piyaṁ manāpaṁ,",
  "ja464:3.2_0": "Anukampakaṁ pāṇasamampi bhattuṁ;",
  "ja464:3.3_0": "Āvāsu kiccesu ca naṁ jahanti,",
  "ja464:3.4_0": "Tasmāhamitthīnaṁ na vissasāmi.",
  "ja464:4.1_0": "Thīnañhi cittaṁ yathā vānarassa,",
  "ja464:4.2_0": "Kannappakannaṁ yathā rukkhachāyā;",
  "ja464:4.3_0": "Calācalaṁ hadayamitthiyānaṁ,",
  "ja464:4.4_0": "Cakkassa nemi viya parivattati.",
  "ja464:5.1_0": "Yadā tā passanti samekkhamānā,",
  "ja464:5.2_0": "Ādeyyarūpaṁ purisassa vittaṁ;",
  "ja464:5.3_0": "Saṇhāhi vācāhi nayanti menaṁ,",
  "ja464:5.4_0": "Kambojakā jalajeneva assaṁ.",
  "ja464:6.1_0": "Yadā na passanti samekkhamānā,",
  "ja464:6.2_0": "Ādeyyarūpaṁ purisassa vittaṁ;",
  "ja464:6.3_0": "Samantato naṁ parivajjayanti,",
  "ja464:6.4_0": "Tiṇṇo nadīpāragatova kullaṁ.",
  "ja464:7.1_0": "Silesūpamā sikhiriva sabbabhakkhā,",
  "ja464:7.2_0": "Tikkhamāyā nadīriva sīghasotā;",
  "ja464:7.3_0": "Sevanti hetā piyamappiyañca,",
  "ja464:7.4_0": "Nāvā yathā orakūlaṁ parañca.",
  "ja464:8.1_0": "Na tā ekassa na dvinnaṁ,",
  "ja464:8.2_0": "āpaṇova pasārito;",
  "ja464:8.3_0": "Yo tā mayhanti maññeyya,",
  "ja464:8.4_0": "vātaṁ jālena bādhaye.",
  "ja464:9.1_0": "Yathā nadī ca pantho ca,",
  "ja464:9.2_0": "pānāgāraṁ sabhā papā;",
  "ja464:9.3_0": "Evaṁ lokitthiyo nāma,",
  "ja464:9.4_0": "velā tāsaṁ na vijjati.",
  "ja464:10.1_0": "Ghatāsanasamā etā,",
  "ja464:10.2_0": "kaṇhasappasirūpamā;",
  "ja464:10.3_0": "Gāvo bahitiṇasseva,",
  "ja464:10.4_0": "omasanti varaṁ varaṁ.",
  "ja464:11.1_0": "Ghatāsanaṁ kuñjaraṁ kaṇhasappaṁ,",
  "ja464:11.2_0": "Muddhābhisittaṁ pamadā ca sabbā;",
  "ja464:11.3_0": "Ete naro niccayato bhajetha,",
  "ja464:11.4_0": "Tesaṁ have dubbidu sabbabhāvo.",
  "ja464:12.1_0": "Naccantavaṇṇā na bahūnaṁ kantā,",
  "ja464:12.2_0": "Na dakkhiṇā pamadā sevitabbā;",
  "ja464:12.3_0": "Na parassa bhariyā na dhanassa hetu,",
  "ja464:12.4_0": "Etitthiyo pañca na sevitabbā”ti.",
  "ja464:13.1_0": "Cūḷakuṇālajātakaṁ paṭhamaṁ."
}