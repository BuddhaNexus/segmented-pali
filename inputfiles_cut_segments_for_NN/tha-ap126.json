{
  "tha-ap126:0.1_0": "Therāpadāna",
  "tha-ap126:0.2_0": "Sereyyavagga",
  "tha-ap126:0.3_0": "4. Gandhodakiyattheraapadāna",
  "tha-ap126:1.1_0": "“Nisajja pāsādavare,",
  "tha-ap126:1.2_0": "vipassiṁ addasaṁ jinaṁ;",
  "tha-ap126:1.3_0": "Kakudhaṁ vilasantaṁva,",
  "tha-ap126:1.4_0": "sabbaññuṁ tamanāsakaṁ.",
  "tha-ap126:2.1_0": "Pāsādassāvidūre ca,",
  "tha-ap126:2.2_0": "gacchati lokanāyako;",
  "tha-ap126:2.3_0": "Pabhā niddhāvate tassa,",
  "tha-ap126:2.4_0": "yathā ca sataraṁsino.",
  "tha-ap126:3.1_0": "Gandhodakañca paggayha,",
  "tha-ap126:3.2_0": "buddhaseṭṭhaṁ samokiriṁ;",
  "tha-ap126:3.3_0": "Tena cittappasādena,",
  "tha-ap126:3.4_0": "tattha kālaṅkato ahaṁ.",
  "tha-ap126:4.1_0": "Ekanavutito kappe,",
  "tha-ap126:4.2_0": "yaṁ gandhodakamākiriṁ;",
  "tha-ap126:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap126:4.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap126:5.1_0": "Ekattiṁse ito kappe,",
  "tha-ap126:5.2_0": "sugandho nāma khattiyo;",
  "tha-ap126:5.3_0": "Sattaratanasampanno,",
  "tha-ap126:5.4_0": "cakkavattī mahabbalo.",
  "tha-ap126:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap126:6.2_0": "…pe…",
  "tha-ap126:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap126:7.1_0": "Itthaṁ sudaṁ āyasmā gandhodakiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap126:8.1_0": "Gandhodakiyattherassāpadānaṁ catutthaṁ."
}