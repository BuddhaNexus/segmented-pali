{
  "mil7.4.7:0.1_0": "Milindapañha",
  "mil7.4.7:0.2_0": "Opammakathāpañha",
  "mil7.4.7:0.3_0": "Pathavīvagga",
  "mil7.4.7:0.4_0": "7. Candaṅgapañha",
  "mil7.4.7:1.1_0": "“Bhante nāgasena,",
  "mil7.4.7:1.1_1": "‘candassa pañca aṅgāni gahetabbānī’ti yaṁ vadesi,",
  "mil7.4.7:1.1_2": "katamāni tāni pañca aṅgāni gahetabbānī”ti?",
  "mil7.4.7:1.2_0": "“Yathā, mahārāja,",
  "mil7.4.7:1.2_1": "cando sukkapakkhe udayanto uttaruttariṁ vaḍḍhati;",
  "mil7.4.7:1.3_0": "evameva kho, mahārāja,",
  "mil7.4.7:1.3_1": "yoginā yogāvacarena ācārasīlaguṇavattappaṭipattiyā āgamādhigame paṭisallāne satipaṭṭhāne indriyesu guttadvāratāya bhojane mattaññutāya jāgariyānuyoge uttaruttariṁ vaḍḍhitabbaṁ.",
  "mil7.4.7:1.4_0": "Idaṁ, mahārāja,",
  "mil7.4.7:1.4_1": "candassa paṭhamaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.4.7:2.1_0": "Puna caparaṁ, mahārāja, cando uḷārādhipati;",
  "mil7.4.7:2.2_0": "evameva kho, mahārāja,",
  "mil7.4.7:2.2_1": "yoginā yogāvacarena uḷārena chandādhipatinā bhavitabbaṁ.",
  "mil7.4.7:2.3_0": "Idaṁ, mahārāja, candassa dutiyaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.4.7:3.1_0": "Puna caparaṁ, mahārāja, cando nisāya carati;",
  "mil7.4.7:3.2_0": "evameva kho, mahārāja,",
  "mil7.4.7:3.2_1": "yoginā yogāvacarena pavivittena bhavitabbaṁ.",
  "mil7.4.7:3.3_0": "Idaṁ, mahārāja, candassa tatiyaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.4.7:4.1_0": "Puna caparaṁ, mahārāja, cando vimānaketu;",
  "mil7.4.7:4.2_0": "evameva kho, mahārāja,",
  "mil7.4.7:4.2_1": "yoginā yogāvacarena sīlaketunā bhavitabbaṁ.",
  "mil7.4.7:4.3_0": "Idaṁ, mahārāja,",
  "mil7.4.7:4.3_1": "candassa catutthaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.4.7:5.1_0": "Puna caparaṁ, mahārāja, cando āyācitapatthito udeti;",
  "mil7.4.7:5.2_0": "evameva kho, mahārāja,",
  "mil7.4.7:5.2_1": "yoginā yogāvacarena āyācitapatthitena kulāni upasaṅkamitabbāni.",
  "mil7.4.7:5.3_0": "Idaṁ, mahārāja,",
  "mil7.4.7:5.3_1": "candassa pañcamaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.4.7:5.4_0": "Bhāsitampetaṁ, mahārāja,",
  "mil7.4.7:5.4_1": "bhagavatā devātidevena saṁyuttanikāyavare ‘candūpamā, bhikkhave, kulāni upasaṅkamatha,",
  "mil7.4.7:5.4_2": "apakasseva kāyaṁ apakassa cittaṁ niccanavakā kulesu appagabbhā’”ti.",
  "mil7.4.7:6.1_0": "Candaṅgapañho sattamo."
}