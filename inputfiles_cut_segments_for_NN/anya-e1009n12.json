{
  "anya-e1009n12:0_0": "Sīla-niddesa",
  "anya-e1009n12:1_0": "291.",
  "anya-e1009n12:2_0": " Pamādaṁ bhayato disvā,",
  "anya-e1009n12:3_0": "A-ppamādañca khemato;",
  "anya-e1009n12:4_0": "Bhāvetha aṭṭhaṅgikaṁ maggaṁ,",
  "anya-e1009n12:5_0": "Esā buddhānusāsanī.",
  "anya-e1009n12:6_0": "292.",
  "anya-e1009n12:7_0": " Hīnena brahma-cariyena,",
  "anya-e1009n12:8_0": "Khattiye upapajjati;",
  "anya-e1009n12:9_0": "Majjhimena ca devattaṁ,",
  "anya-e1009n12:10_0": "Uttamena vi-sujjhati.",
  "anya-e1009n12:11_0": "Ka.",
  "anya-e1009n12:12_0": " Nagare bandhumatiyā,",
  "anya-e1009n12:13_0": "Bandhumā nāma khattiyo;",
  "anya-e1009n12:14_0": "Divase puṇṇamāya so,",
  "anya-e1009n12:15_0": "Upagacchi uposathaṁ.",
  "anya-e1009n12:16_0": "Kha.",
  "anya-e1009n12:17_0": " Ahaṁ tena samayena,",
  "anya-e1009n12:18_0": "Gumbha-dāsī ahaṁ tahiṁ;",
  "anya-e1009n12:19_0": "Disvā sa-rājakaṁ senaṁ,",
  "anya-e1009n12:20_0": "Evāhaṁ cintayiṁ tadā.",
  "anya-e1009n12:21_0": "Ga.",
  "anya-e1009n12:22_0": " Rājāpi rajjaṁ chaṭṭetvā,",
  "anya-e1009n12:23_0": "Upagacchi uposathaṁ;",
  "anya-e1009n12:24_0": "Sa-phalaṁ nūna taṁ kammaṁ,",
  "anya-e1009n12:25_0": "Jana-kāyo pamodito.",
  "anya-e1009n12:26_0": "Gha.",
  "anya-e1009n12:27_0": " Yoniso paccavekkhitvā,",
  "anya-e1009n12:28_0": "Duggaccañca daliddataṁ;",
  "anya-e1009n12:29_0": "Mānasaṁ sampahaṁsitvā,",
  "anya-e1009n12:30_0": "Upagacchimu posathaṁ.",
  "anya-e1009n12:31_0": "Ṅa.",
  "anya-e1009n12:32_0": " Ahaṁ uposathaṁ katvā,",
  "anya-e1009n12:33_0": "Sammā-sambuddhasāsane;",
  "anya-e1009n12:34_0": "Tena kammena su-katena,",
  "anya-e1009n12:35_0": "Tāvatiṁsaṁ agacchahaṁ.",
  "anya-e1009n12:36_0": "Ca.",
  "anya-e1009n12:37_0": " Tattha me su-kataṁ byamhaṁ,",
  "anya-e1009n12:38_0": "Ubbha-yojanamuggataṁ;",
  "anya-e1009n12:39_0": "Kūṭāgāra-varūpetaṁ,",
  "anya-e1009n12:40_0": "Mahāsanasu-bhūsitaṁ.",
  "anya-e1009n12:41_0": "Cha.",
  "anya-e1009n12:42_0": " Accharā sata-sahassā,",
  "anya-e1009n12:43_0": "Upatiṭṭhanti maṁ sadā;",
  "anya-e1009n12:44_0": "Aññe deve atikkamma,",
  "anya-e1009n12:45_0": "Atirocāmi sabbadā.",
  "anya-e1009n12:46_0": "Ja.",
  "anya-e1009n12:47_0": " Catusaṭṭhi-deva-rājūnaṁ ,",
  "anya-e1009n12:48_0": "Mahesittamakārayiṁ;",
  "anya-e1009n12:49_0": "Tesaṭṭhi-cakkavattinaṁ,",
  "anya-e1009n12:50_0": "Mahesittamakārayiṁ.",
  "anya-e1009n12:51_0": "Jha.",
  "anya-e1009n12:52_0": " Suvaṇṇa-vaṇā hutvāna,",
  "anya-e1009n12:53_0": "Bhavesu saṁsarāmahaṁ;",
  "anya-e1009n12:54_0": "Sabbattha pavarā homi,",
  "anya-e1009n12:55_0": "Uposathassidaṁ phalaṁ.",
  "anya-e1009n12:56_0": "Ña.",
  "anya-e1009n12:57_0": " Hatthi-yānaṁ assa-yānaṁ,",
  "anya-e1009n12:58_0": "Ratha-yānañca sivikaṁ;",
  "anya-e1009n12:59_0": "Labhāmi sabbametampi,",
  "anya-e1009n12:60_0": "Uposathassidaṁ phalaṁ.",
  "anya-e1009n12:61_0": "Ṭa.",
  "anya-e1009n12:62_0": " Soṇṇa-mayaṁ rūpi-mayaṁ,",
  "anya-e1009n12:63_0": "Athopi phalikā-mayaṁ;",
  "anya-e1009n12:64_0": "Lohitaṅga-mayañceva,",
  "anya-e1009n12:65_0": "Sabbaṁ paṭilabhāmahaṁ.",
  "anya-e1009n12:66_0": "Ṭha.",
  "anya-e1009n12:67_0": " Koseyya-kambaliyāni ,",
  "anya-e1009n12:68_0": "Khoma-kappāsikāni ca;",
  "anya-e1009n12:69_0": "Mahagghāni ca vatthāni,",
  "anya-e1009n12:70_0": "Sabbaṁ paṭilabhāmahaṁ.",
  "anya-e1009n12:71_0": "Ḍa.",
  "anya-e1009n12:72_0": " Annaṁ pānaṁ khādanīyaṁ,",
  "anya-e1009n12:73_0": "Vatthaṁ senāsanāni ca;",
  "anya-e1009n12:74_0": "Sabbametaṁ paṭilabhe,",
  "anya-e1009n12:75_0": "Uposathassidaṁ phalaṁ.",
  "anya-e1009n12:76_0": "Ḍha.",
  "anya-e1009n12:77_0": " Vara-gandhañca mālañca,",
  "anya-e1009n12:78_0": "Cuṇṇakañcha vilepanaṁ;",
  "anya-e1009n12:79_0": "Sabbametaṁ paṭilabhe,",
  "anya-e1009n12:80_0": "Uposathassidaṁ phalaṁ.",
  "anya-e1009n12:81_0": "Ṇa.",
  "anya-e1009n12:82_0": " Kūṭāgārañca pāsādaṁ,",
  "anya-e1009n12:83_0": "Maṇḍapaṁ hammiyaṁ guhaṁ;",
  "anya-e1009n12:84_0": "Sabbametaṁ paṭilabhe,",
  "anya-e1009n12:85_0": "Uposathassidaṁ phalaṁ.",
  "anya-e1009n12:86_0": "Ta.",
  "anya-e1009n12:87_0": " Jātiyā satta-vassāhaṁ,",
  "anya-e1009n12:88_0": "Pabbajiṁ ana-gāriyaṁ;",
  "anya-e1009n12:89_0": "Aḍḍha-māse a-sappatte,",
  "anya-e1009n12:90_0": "Ara hattaṁ apāpuṇiṁ.",
  "anya-e1009n12:91_0": "Tha.",
  "anya-e1009n12:92_0": " Kilesā jhāpitā mayhaṁ,",
  "anya-e1009n12:93_0": "Bhavā sabbe samūhatā;",
  "anya-e1009n12:94_0": "Sabbāsava-parikkhīṇā,",
  "anya-e1009n12:95_0": "Natthi dāni puna-bbhavo.",
  "anya-e1009n12:96_0": "Da.",
  "anya-e1009n12:97_0": " Eka-navutito kappe,",
  "anya-e1009n12:98_0": "Yaṁ kammamakariṁ tadā;",
  "anya-e1009n12:99_0": "Duggatiṁ nābhijānāmi,",
  "anya-e1009n12:100_0": "Uposathassidaṁ phalaṁ.",
  "anya-e1009n12:101_0": "Dha.",
  "anya-e1009n12:102_0": " Svāgataṁ vata me āsi,",
  "anya-e1009n12:103_0": "Mama buddhassa santike;",
  "anya-e1009n12:104_0": "Tisso vijjā anu-pattā,",
  "anya-e1009n12:105_0": "Kataṁ buddhassa sāsanaṁ.",
  "anya-e1009n12:106_0": "Na.",
  "anya-e1009n12:107_0": " Paṭisambhidā catasso,",
  "anya-e1009n12:108_0": "Vimokāpi ca aṭṭhime;",
  "anya-e1009n12:109_0": "Chaḷābhiññā sacchikatā,",
  "anya-e1009n12:110_0": "Kataṁ buddhassa sāsanaṁ.",
  "anya-e1009n12:111_0": "293.",
  "anya-e1009n12:112_0": " Ñātīnañca piyo hoti,",
  "anya-e1009n12:113_0": "Mittesu ca virocati;",
  "anya-e1009n12:114_0": "Kāyassa bhedā su-gatiṁ,",
  "anya-e1009n12:115_0": "Upapajjati sīlavā.",
  "anya-e1009n12:116_0": "294.",
  "anya-e1009n12:117_0": " Nibbānaṁ patthayantena samādinnaṁ,",
  "anya-e1009n12:118_0": "Pañca-sīlampi adhi-sīlaṁ;",
  "anya-e1009n12:119_0": "Dasa-sīlampi adhi-sīlameva.",
  "anya-e1009n12:120_0": "Ka.",
  "anya-e1009n12:121_0": " Nagare candavatiyā,",
  "anya-e1009n12:122_0": "Bhaṭako āsahaṁ tadā;",
  "anya-e1009n12:123_0": "Para-kammāyane yutto,",
  "anya-e1009n12:124_0": "Pabbajjaṁ na labhāmahaṁ.",
  "anya-e1009n12:125_0": "Kha.",
  "anya-e1009n12:126_0": " Mahandhakāra-pihitā ,",
  "anya-e1009n12:127_0": "Tividhaggīhi ḍayhare;",
  "anya-e1009n12:128_0": "Kena nukho upāyena,",
  "anya-e1009n12:129_0": "Vi-saṁyutto bhave ahaṁ.",
  "anya-e1009n12:130_0": "Ga.",
  "anya-e1009n12:131_0": " Deyyadhammo ca me natthi,",
  "anya-e1009n12:132_0": "Bhaṭako dukkhito ahaṁ;",
  "anya-e1009n12:133_0": "Yaṁ nūnāhaṁ pañca-sīlaṁ,",
  "anya-e1009n12:134_0": "Rakkheyyaṁ paripūrayaṁ.",
  "anya-e1009n12:135_0": "Gha.",
  "anya-e1009n12:136_0": " Anomadassissa munino,",
  "anya-e1009n12:137_0": "Nisabho nāma sāvako;",
  "anya-e1009n12:138_0": "Tamahaṁ upasaṅkamma,",
  "anya-e1009n12:139_0": "Pañca-sikkhāpadaggahiṁ.",
  "anya-e1009n12:140_0": "Ṅa.",
  "anya-e1009n12:141_0": " Vassa-sata-sahassāni ,",
  "anya-e1009n12:142_0": "Āyu vijjati tāvade;",
  "anya-e1009n12:143_0": "Tāvatā pañca-sīlāni,",
  "anya-e1009n12:144_0": "Paripuṇṇāni gopayiṁ.",
  "anya-e1009n12:145_0": "Ca.",
  "anya-e1009n12:146_0": " Maccu-kālamhi sampatte,",
  "anya-e1009n12:147_0": "Devā assāsayanti maṁ;",
  "anya-e1009n12:148_0": "Ratho sahassa-yutto te,",
  "anya-e1009n12:149_0": "Mārisassa upaṭṭhito.",
  "anya-e1009n12:150_0": "Cha.",
  "anya-e1009n12:151_0": " Vattante carime citte,",
  "anya-e1009n12:152_0": "Mama sīlaṁ anussariṁ;",
  "anya-e1009n12:153_0": "Tena kammena su-katena,",
  "anya-e1009n12:154_0": "Tāvatiṁsaṁ agacchahaṁ.",
  "anya-e1009n12:155_0": "Ja.",
  "anya-e1009n12:156_0": " Tiṁsakhattuñca devindo,",
  "anya-e1009n12:157_0": "Deva-rajjamakārayiṁ;",
  "anya-e1009n12:158_0": "Dibba-sukhaṁ anubhaviṁ,",
  "anya-e1009n12:159_0": "Accharāhi purakkhatto.",
  "anya-e1009n12:160_0": "Jha.",
  "anya-e1009n12:161_0": " Pañca-sattatikhatttu-ñca ,",
  "anya-e1009n12:162_0": "Cakkavattī ahosahaṁ;",
  "anya-e1009n12:163_0": "Padesa-rajjaṁ vipulaṁ,",
  "anya-e1009n12:164_0": "Gaṇanāto a-saṅkhayaṁ.",
  "anya-e1009n12:165_0": "Ña.",
  "anya-e1009n12:166_0": " Deva-lokā cavitvāna,",
  "anya-e1009n12:167_0": "Sukka-mūlena codito;",
  "anya-e1009n12:168_0": "Pure vesāliyaṁ jāto,",
  "anya-e1009n12:169_0": "Mahā-kule su-aḍḍhake.",
  "anya-e1009n12:170_0": "Ṭa.",
  "anya-e1009n12:171_0": " Vassūpanāyike kāle,",
  "anya-e1009n12:172_0": "Dibbante jina-sāsane;",
  "anya-e1009n12:173_0": "Mātā ca me pitā ceva,",
  "anya-e1009n12:174_0": "Pañca-sikkhāpadaggahuṁ.",
  "anya-e1009n12:175_0": "Ṭha.",
  "anya-e1009n12:176_0": " Saha sutvānahaṁ sīlaṁ,",
  "anya-e1009n12:177_0": "Mama sīlaṁ anussariṁ;",
  "anya-e1009n12:178_0": "Ekāsane nisīditvā,",
  "anya-e1009n12:179_0": "Arahattamapāpuṇiṁ.",
  "anya-e1009n12:180_0": "Ḍa.",
  "anya-e1009n12:181_0": " Jātiyā pañca-vassena,",
  "anya-e1009n12:182_0": "Arahattamapāpuṇiṁ;",
  "anya-e1009n12:183_0": "Upasampādayi buddho,",
  "anya-e1009n12:184_0": "Guṇamaññāya cakkhumā.",
  "anya-e1009n12:185_0": "Ḍha.",
  "anya-e1009n12:186_0": " Paripuṇṇāni gopetvā,",
  "anya-e1009n12:187_0": "Pañca-sikkhāpadānahaṁ;",
  "anya-e1009n12:188_0": "A-parimeyyito kappe,",
  "anya-e1009n12:189_0": "Vinipātaṁ na gacchahaṁ.",
  "anya-e1009n12:190_0": "Ṇa.",
  "anya-e1009n12:191_0": " Svāhaṁ yasamanubhaviṁ,",
  "anya-e1009n12:192_0": "Tesaṁ sīlāna vāhasā;",
  "anya-e1009n12:193_0": "Kappa-koṭipi kittento,",
  "anya-e1009n12:194_0": "Kittaye eka-desakaṁ.",
  "anya-e1009n12:195_0": "Ta.",
  "anya-e1009n12:196_0": " Pañca-sīlāni gopetvā,",
  "anya-e1009n12:197_0": "Tayo hetū labhāmahaṁ;",
  "anya-e1009n12:198_0": "Dīghāyuko mahā-bhogo,",
  "anya-e1009n12:199_0": "Tikkha-pañño bhavāmahaṁ.",
  "anya-e1009n12:200_0": "Tha.",
  "anya-e1009n12:201_0": " Saṁkittento ca sabbesaṁ,",
  "anya-e1009n12:202_0": "Adhi-mattañca porisaṁ;",
  "anya-e1009n12:203_0": "Bhavābhave saṁsaritvā,",
  "anya-e1009n12:204_0": "Ete ṭhāne labhamahaṁ.",
  "anya-e1009n12:205_0": "Da.",
  "anya-e1009n12:206_0": " A-parimeyya-sīlesu ,",
  "anya-e1009n12:207_0": "Vattantā jina-sāvakā;",
  "anya-e1009n12:208_0": "Bhavesu yadi rajjeyyuṁ,",
  "anya-e1009n12:209_0": "Vipāko kīdiso bhave.",
  "anya-e1009n12:210_0": "Dha.",
  "anya-e1009n12:211_0": " Su-ciṇṇaṁ me pañca-sīlaṁ,",
  "anya-e1009n12:212_0": "Bhaṭakena tapassinā;",
  "anya-e1009n12:213_0": "Tena sīlenahaṁ ajja,",
  "anya-e1009n12:214_0": "Mocayiṁ sabba-bandhanā.",
  "anya-e1009n12:215_0": "Na.",
  "anya-e1009n12:216_0": " A-parimeyyito kappe,",
  "anya-e1009n12:217_0": "Pañca-sīlāni gopayiṁ;",
  "anya-e1009n12:218_0": "Duggatiṁ nābhijānāmi,",
  "anya-e1009n12:219_0": "Pañca-sīlānidaṁ phalaṁ.",
  "anya-e1009n12:220_0": "Pa.",
  "anya-e1009n12:221_0": " Paṭisambhidā catasso,",
  "anya-e1009n12:222_0": "Vimokkhāpi ca aṭṭhime;",
  "anya-e1009n12:223_0": "Chaḷābhiññā sacchikatā,",
  "anya-e1009n12:224_0": "Kataṁ buddhassa sāsanaṁ.",
  "anya-e1009n12:225_0": "Ka.",
  "anya-e1009n12:226_0": " Taṁ namassanti te vijjā,",
  "anya-e1009n12:227_0": "Sabbe bhūmā ca khattiyā;",
  "anya-e1009n12:228_0": "Cattāro ca mahā-rājā,",
  "anya-e1009n12:229_0": "Tidasā ca yasassino;",
  "anya-e1009n12:230_0": "Atha ko nāma so yakkho,",
  "anya-e1009n12:231_0": "Yaṁ tvaṁ sakka namassasi.",
  "anya-e1009n12:232_0": "Kha.",
  "anya-e1009n12:233_0": " Maṁ namassanti te-vijjā,",
  "anya-e1009n12:234_0": "Sabbe bhūmā ca khattiyā;",
  "anya-e1009n12:235_0": "Cattāro ca mahā-rājā,",
  "anya-e1009n12:236_0": "Tidasā ca yasassino.",
  "anya-e1009n12:237_0": "Ga.",
  "anya-e1009n12:238_0": " Ahañca sīla-sampanne,",
  "anya-e1009n12:239_0": "Ciraratta-samāhite;",
  "anya-e1009n12:240_0": "Sammā pabbajite vande,",
  "anya-e1009n12:241_0": "Brahma-cariya parāyane.",
  "anya-e1009n12:242_0": "Gha.",
  "anya-e1009n12:243_0": " Ye gahaṭṭhā puñña-karā,",
  "anya-e1009n12:244_0": "Sīlavanto upāsakā;",
  "anya-e1009n12:245_0": "Dhammena dāraṁ posenti,",
  "anya-e1009n12:246_0": "Te namassāmi mātali.",
  "anya-e1009n12:247_0": "Ṅa.",
  "anya-e1009n12:248_0": " Seṭṭhā hi kira lokasmiṁ,",
  "anya-e1009n12:249_0": "Ye tvaṁ sakka namassasi;",
  "anya-e1009n12:250_0": "Ahampi te namassāmi,",
  "anya-e1009n12:251_0": "Ye namassasi vāsava.",
  "anya-e1009n12:252_0": "1.",
  "anya-e1009n12:253_0": " Pālitattheranāgena ,",
  "anya-e1009n12:254_0": "Visuddhārāmavāsinā;",
  "anya-e1009n12:255_0": "Suticchitānamatthāya,",
  "anya-e1009n12:256_0": "Katā naradakkhadīpanī.",
  "anya-e1009n12:257_0": "2.",
  "anya-e1009n12:258_0": " Pubbācariya-sīhānaṁ ,",
  "anya-e1009n12:259_0": "Ālambitvāna nissayaṁ;",
  "anya-e1009n12:260_0": "Pālito nāma yo thero,",
  "anya-e1009n12:261_0": "Imaṁ gantha su-lekhanī;",
  "anya-e1009n12:262_0": "Sundarameva passituṁ,",
  "anya-e1009n12:263_0": "Yuñjeyyāthīdha sādhave.",
  "anya-e1009n12:264_0": "‘‘Chappadikā’’.",
  "anya-e1009n12:265_0": "3.",
  "anya-e1009n12:266_0": " Imaṁ ganthaṁ vācuggato,",
  "anya-e1009n12:267_0": "Sace bhavasi māṇava;",
  "anya-e1009n12:268_0": "Puṇnamāyaṁ yathā cando,",
  "anya-e1009n12:269_0": "Ati-suddho virocati;",
  "anya-e1009n12:270_0": "Tatheva tvaṁ puṇṇa-mano,",
  "anya-e1009n12:271_0": "Viroca siriyā dhuvaṁ.",
  "anya-e1009n12:272_0": "4.",
  "anya-e1009n12:273_0": " Su-niṭṭhito ayaṁ gantho,",
  "anya-e1009n12:274_0": "Sakkarāje dajhamphiye;",
  "anya-e1009n12:275_0": "Poṭṭhapādamhi sūramhi,",
  "anya-e1009n12:276_0": "Kālapakkhe catuddasiṁ.",
  "anya-e1009n12:277_0": "5.",
  "anya-e1009n12:278_0": " Sañcitetaṁ mayā puññaṁ,",
  "anya-e1009n12:279_0": "Taṁ-kammena varena ca;",
  "anya-e1009n12:280_0": "Ciraṁ tiṭṭhatu saddhammo,",
  "anya-e1009n12:281_0": "A-verā hontu pāṇino.",
  "anya-e1009n12:282_0": "6.",
  "anya-e1009n12:283_0": " Imaṁ ganthaṁ passitvāna,",
  "anya-e1009n12:284_0": "Hontu sabbepi jantuno;",
  "anya-e1009n12:285_0": "Sukhitā dhammikā ñāṇī,",
  "anya-e1009n12:286_0": "Dhammaṁ pāletu patthivo.",
  "anya-e1009n12:287_0": "7.",
  "anya-e1009n12:288_0": " Nibbānaṁ patthayantena,",
  "anya-e1009n12:289_0": "Sīlaṁ rakkhantu sajjanā;",
  "anya-e1009n12:290_0": "Ñatvā dhammaṁ sukhāvahaṁ,",
  "anya-e1009n12:291_0": "Pāpuṇantu anāsavaṁ.",
  "anya-e1009n12:292_0": "8.",
  "anya-e1009n12:293_0": " Aṭṭha-kaṇḍa-maṇḍitāya ,",
  "anya-e1009n12:294_0": "Dakkhaya attha-dīpako;",
  "anya-e1009n12:295_0": "Nara-sāro ayaṁ gantho,",
  "anya-e1009n12:296_0": "Cira-kālaṁ patiṭṭhatu.",
  "anya-e1009n12:297_0": "9.",
  "anya-e1009n12:298_0": " Yāvatā canda-sūriyā,",
  "anya-e1009n12:299_0": "Nāgaccheyyuṁ mahī-tale;",
  "anya-e1009n12:300_0": "Pamoditā imaṁ ganthaṁ,",
  "anya-e1009n12:301_0": "Dissantu naya-kovidā.",
  "anya-e1009n12:302_0": "10.",
  "anya-e1009n12:303_0": " Sammā chandenimaṁ gantha,",
  "anya-e1009n12:304_0": "Vācentā pariyāpuṇā;",
  "anya-e1009n12:305_0": "Pasannenānāyāsena,",
  "anya-e1009n12:306_0": "Patvā sukhena kovidaṁ.",
  "anya-e1009n12:307_0": "11.",
  "anya-e1009n12:308_0": " Candādiccāva ākāse,",
  "anya-e1009n12:309_0": "Bahussutehi sampadā;",
  "anya-e1009n12:310_0": "Visesa-puggalā hutvā,",
  "anya-e1009n12:311_0": "Pappontu amataṁ padaṁ.",
  "anya-e1009n12:312_0": "12.",
  "anya-e1009n12:313_0": " Ukkaṭṭha-dhamma-dānena ,",
  "anya-e1009n12:314_0": "Pāpuṇeyyamanuttaraṁ;",
  "anya-e1009n12:315_0": "Liṅga-sampatti-medhāvī,",
  "anya-e1009n12:316_0": "Takkī-paññā su-pesalī."
}