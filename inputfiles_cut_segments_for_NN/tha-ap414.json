{
  "tha-ap414:0.1_0": "Therāpadāna",
  "tha-ap414:0.2_0": "Bhaddālivagga",
  "tha-ap414:0.3_0": "2. Ekachattiyattheraapadāna",
  "tha-ap414:1.1_0": "“Candabhāgānadītīre,",
  "tha-ap414:1.2_0": "assamo sukato mama;",
  "tha-ap414:1.3_0": "Susuddhapulinākiṇṇo,",
  "tha-ap414:1.4_0": "pannasālā sumāpitā.",
  "tha-ap414:2.1_0": "Uttānakūlā nadikā,",
  "tha-ap414:2.2_0": "supatitthā manoramā;",
  "tha-ap414:2.3_0": "Macchakacchapasampannā,",
  "tha-ap414:2.4_0": "susumāranisevitā.",
  "tha-ap414:3.1_0": "Acchā dīpī ca mayūrā,",
  "tha-ap414:3.2_0": "karavīkā ca sāḷikā;",
  "tha-ap414:3.3_0": "Kūjanti sabbadā ete,",
  "tha-ap414:3.4_0": "sobhayantā mamassamaṁ.",
  "tha-ap414:4.1_0": "Kokilā mañjubhāṇī ca,",
  "tha-ap414:4.2_0": "haṁsā ca madhurassarā;",
  "tha-ap414:4.3_0": "Abhikūjanti te tattha,",
  "tha-ap414:4.4_0": "sobhayantā mamassamaṁ.",
  "tha-ap414:5.1_0": "Sīhā byagghā varāhā ca,",
  "tha-ap414:5.2_0": "accha kokataracchakā;",
  "tha-ap414:5.3_0": "Giriduggamhi nādenti,",
  "tha-ap414:5.4_0": "sobhayantā mamassamaṁ.",
  "tha-ap414:6.1_0": "Eṇīmigā ca sarabhā,",
  "tha-ap414:6.2_0": "bheraṇḍā sūkarā bahū;",
  "tha-ap414:6.3_0": "Giriduggamhi nādenti,",
  "tha-ap414:6.4_0": "sobhayantā mamassamaṁ.",
  "tha-ap414:7.1_0": "Uddālakā campakā ca,",
  "tha-ap414:7.2_0": "pāṭalī sinduvārakā;",
  "tha-ap414:7.3_0": "Atimuttā asokā ca,",
  "tha-ap414:7.4_0": "sobhayanti mamassamaṁ.",
  "tha-ap414:8.1_0": "Aṅkolā yūthikā ceva,",
  "tha-ap414:8.2_0": "sattalī bimbijālikā;",
  "tha-ap414:8.3_0": "Kaṇikārā ca pupphanti,",
  "tha-ap414:8.4_0": "sobhayantā mamassamaṁ.",
  "tha-ap414:9.1_0": "Nāgā sālā ca saḷalā,",
  "tha-ap414:9.2_0": "puṇḍarīkettha pupphitā;",
  "tha-ap414:9.3_0": "Dibbagandhaṁ sampavantā,",
  "tha-ap414:9.4_0": "sobhayanti mamassamaṁ.",
  "tha-ap414:10.1_0": "Ajjunā asanā cettha,",
  "tha-ap414:10.2_0": "mahānāmā ca pupphitā;",
  "tha-ap414:10.3_0": "Sālā ca kaṅgupupphā ca,",
  "tha-ap414:10.4_0": "sobhayanti mamassamaṁ.",
  "tha-ap414:11.1_0": "Ambā jambū ca tilakā,",
  "tha-ap414:11.2_0": "nimbā ca sālakalyāṇī;",
  "tha-ap414:11.3_0": "Dibbagandhaṁ sampavantā,",
  "tha-ap414:11.4_0": "sobhayanti mamassamaṁ.",
  "tha-ap414:12.1_0": "Asokā ca kapiṭṭhā ca,",
  "tha-ap414:12.2_0": "girimālettha pupphitā;",
  "tha-ap414:12.3_0": "Dibbagandhaṁ sampavantā,",
  "tha-ap414:12.4_0": "sobhayanti mamassamaṁ.",
  "tha-ap414:13.1_0": "Kadambā kadalī ceva,",
  "tha-ap414:13.2_0": "isimuggā ca ropitā;",
  "tha-ap414:13.3_0": "Dhuvaṁ phalāni dhārenti,",
  "tha-ap414:13.4_0": "sobhayantā mamassamaṁ.",
  "tha-ap414:14.1_0": "Harītakā āmalakā,",
  "tha-ap414:14.2_0": "ambajambuvibhītakā;",
  "tha-ap414:14.3_0": "Kolā bhallātakā billā,",
  "tha-ap414:14.4_0": "phalino mama assame.",
  "tha-ap414:15.1_0": "Avidūre pokkharaṇī,",
  "tha-ap414:15.2_0": "supatitthā manoramā;",
  "tha-ap414:15.3_0": "Mandālakehi sañchannā,",
  "tha-ap414:15.4_0": "padumuppalakehi ca.",
  "tha-ap414:16.1_0": "Gabbhaṁ gaṇhanti padumā,",
  "tha-ap414:16.2_0": "aññe pupphanti kesarī;",
  "tha-ap414:16.3_0": "Opattakaṇṇikā ceva,",
  "tha-ap414:16.4_0": "pupphanti mama assame.",
  "tha-ap414:17.1_0": "Pāṭhīnā pāvusā macchā,",
  "tha-ap414:17.2_0": "balajā muñjarohitā;",
  "tha-ap414:17.3_0": "Acchodakamhi vicaraṁ,",
  "tha-ap414:17.4_0": "sobhayanti mamassamaṁ.",
  "tha-ap414:18.1_0": "Nayitā ambagandhī ca,",
  "tha-ap414:18.2_0": "anukūle ca ketakā;",
  "tha-ap414:18.3_0": "Dibbagandhaṁ sampavantā,",
  "tha-ap414:18.4_0": "sobhayanti mamassamaṁ.",
  "tha-ap414:19.1_0": "Madhu bhisamhā savati,",
  "tha-ap414:19.2_0": "khīrasappi muḷālibhi;",
  "tha-ap414:19.3_0": "Dibbagandhaṁ sampavantā,",
  "tha-ap414:19.4_0": "sobhayanti mamassamaṁ.",
  "tha-ap414:20.1_0": "Pulinā sobhanā tattha,",
  "tha-ap414:20.2_0": "ākiṇṇā jalasevitā;",
  "tha-ap414:20.3_0": "Opupphā pupphitā senti,",
  "tha-ap414:20.4_0": "sobhayantā mamassamaṁ.",
  "tha-ap414:21.1_0": "Jaṭābhārena bharitā,",
  "tha-ap414:21.2_0": "ajinuttaravāsanā;",
  "tha-ap414:21.3_0": "Vākacīradharā sabbe,",
  "tha-ap414:21.4_0": "sobhayanti mamassamaṁ.",
  "tha-ap414:22.1_0": "Yugamattamapekkhantā,",
  "tha-ap414:22.2_0": "nipakā santavuttino;",
  "tha-ap414:22.3_0": "Kāmabhoge anapekkhā,",
  "tha-ap414:22.4_0": "vasanti mama assame.",
  "tha-ap414:23.1_0": "Parūḷhakacchanakhalomā,",
  "tha-ap414:23.2_0": "paṅkadantā rajassirā;",
  "tha-ap414:23.3_0": "Rajojalladharā sabbe,",
  "tha-ap414:23.4_0": "vasanti mama assame.",
  "tha-ap414:24.1_0": "Abhiññāpāramippattā,",
  "tha-ap414:24.2_0": "antalikkhacarā ca te;",
  "tha-ap414:24.3_0": "Uggacchantā nabhaṁ ete,",
  "tha-ap414:24.4_0": "sobhayanti mamassamaṁ.",
  "tha-ap414:25.1_0": "Tehi sissehi parivuto,",
  "tha-ap414:25.2_0": "vasāmi vipine tadā;",
  "tha-ap414:25.3_0": "Rattindivaṁ na jānāmi,",
  "tha-ap414:25.4_0": "sadā jhānasamappito.",
  "tha-ap414:26.1_0": "Bhagavā tamhi samaye,",
  "tha-ap414:26.2_0": "atthadassī mahāmuni;",
  "tha-ap414:26.3_0": "Tamandhakāraṁ nāsento,",
  "tha-ap414:26.4_0": "uppajji lokanāyako.",
  "tha-ap414:27.1_0": "Atha aññataro sisso,",
  "tha-ap414:27.2_0": "āgacchi mama santikaṁ;",
  "tha-ap414:27.3_0": "Mante ajjhetukāmo so,",
  "tha-ap414:27.4_0": "chaḷaṅgaṁ nāma lakkhaṇaṁ.",
  "tha-ap414:28.1_0": "Buddho loke samuppanno,",
  "tha-ap414:28.2_0": "atthadassī mahāmuni;",
  "tha-ap414:28.3_0": "Catusaccaṁ pakāsento,",
  "tha-ap414:28.4_0": "deseti amataṁ padaṁ.",
  "tha-ap414:29.1_0": "Tuṭṭhahaṭṭho pamudito,",
  "tha-ap414:29.2_0": "dhammantaragatāsayo;",
  "tha-ap414:29.3_0": "Assamā abhinikkhamma,",
  "tha-ap414:29.4_0": "idaṁ vacanamabraviṁ.",
  "tha-ap414:30.1_0": "‘Buddho loke samuppanno,",
  "tha-ap414:30.2_0": "dvattiṁsavaralakkhaṇo;",
  "tha-ap414:30.3_0": "Etha sabbe gamissāma,",
  "tha-ap414:30.4_0": "sammāsambuddhasantikaṁ’.",
  "tha-ap414:31.1_0": "Ovādapaṭikarā te,",
  "tha-ap414:31.2_0": "sadhamme pāramiṁ gatā;",
  "tha-ap414:31.3_0": "Sādhūti sampaṭicchiṁsu,",
  "tha-ap414:31.4_0": "uttamatthagavesakā.",
  "tha-ap414:32.1_0": "Jaṭābhārabharitā te,",
  "tha-ap414:32.2_0": "ajinuttaravāsanā;",
  "tha-ap414:32.3_0": "Uttamatthaṁ gavesantā,",
  "tha-ap414:32.4_0": "nikkhamiṁsu vanā tadā.",
  "tha-ap414:33.1_0": "Bhagavā tamhi samaye,",
  "tha-ap414:33.2_0": "atthadassī mahāyaso;",
  "tha-ap414:33.3_0": "Catusaccaṁ pakāsento,",
  "tha-ap414:33.4_0": "deseti amataṁ padaṁ.",
  "tha-ap414:34.1_0": "Setacchattaṁ gahetvāna,",
  "tha-ap414:34.2_0": "buddhaseṭṭhassa dhārayiṁ;",
  "tha-ap414:34.3_0": "Ekāhaṁ dhārayitvāna,",
  "tha-ap414:34.4_0": "buddhaseṭṭhaṁ avandahaṁ.",
  "tha-ap414:35.1_0": "Atthadassī tu bhagavā,",
  "tha-ap414:35.2_0": "lokajeṭṭho narāsabho;",
  "tha-ap414:35.3_0": "Bhikkhusaṅghe nisīditvā,",
  "tha-ap414:35.4_0": "imā gāthā abhāsatha.",
  "tha-ap414:36.1_0": "‘Yo me chattaṁ adhāresi,",
  "tha-ap414:36.2_0": "pasanno sehi pāṇibhi;",
  "tha-ap414:36.3_0": "Tamahaṁ kittayissāmi,",
  "tha-ap414:36.4_0": "suṇātha mama bhāsato.",
  "tha-ap414:37.1_0": "Imassa jāyamānassa,",
  "tha-ap414:37.2_0": "devatte atha mānuse;",
  "tha-ap414:37.3_0": "Dhāressati sadā chattaṁ,",
  "tha-ap414:37.4_0": "chattadānassidaṁ phalaṁ.",
  "tha-ap414:38.1_0": "Sattasattatikappāni,",
  "tha-ap414:38.2_0": "devaloke ramissati;",
  "tha-ap414:38.3_0": "Sahassakkhattuṁ rājā ca,",
  "tha-ap414:38.4_0": "cakkavattī bhavissati.",
  "tha-ap414:39.1_0": "Sattasattatikkhattuñca,",
  "tha-ap414:39.2_0": "devarajjaṁ karissati;",
  "tha-ap414:39.3_0": "Padesarajjaṁ vipulaṁ,",
  "tha-ap414:39.4_0": "gaṇanāto asaṅkhiyaṁ.",
  "tha-ap414:40.1_0": "Aṭṭhārase kappasate,",
  "tha-ap414:40.2_0": "gotamo sakyapuṅgavo;",
  "tha-ap414:40.3_0": "Tamandhakāraṁ nāsento,",
  "tha-ap414:40.4_0": "uppajjissati cakkhumā.",
  "tha-ap414:41.1_0": "Tassa dhammesu dāyādo,",
  "tha-ap414:41.2_0": "oraso dhammanimmito;",
  "tha-ap414:41.3_0": "Sabbāsave pariññāya,",
  "tha-ap414:41.4_0": "viharissatināsavo’.",
  "tha-ap414:42.1_0": "Yato ahaṁ kammamakaṁ,",
  "tha-ap414:42.2_0": "chattaṁ buddhassa dhārayaṁ;",
  "tha-ap414:42.3_0": "Etthantare na jānāmi,",
  "tha-ap414:42.4_0": "setacchattaṁ adhāritaṁ.",
  "tha-ap414:43.1_0": "Idaṁ pacchimakaṁ mayhaṁ,",
  "tha-ap414:43.2_0": "carimo vattate bhavo;",
  "tha-ap414:43.3_0": "Chattadhāraṇamajjāpi,",
  "tha-ap414:43.4_0": "vattate niccakālikaṁ.",
  "tha-ap414:44.1_0": "Aho me sukataṁ kammaṁ,",
  "tha-ap414:44.2_0": "atthadassissa tādino;",
  "tha-ap414:44.3_0": "Sabbāsavā parikkhīṇā,",
  "tha-ap414:44.4_0": "natthi dāni punabbhavo.",
  "tha-ap414:45.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap414:45.2_0": "…pe…",
  "tha-ap414:45.3_0": "viharāmi anāsavo.",
  "tha-ap414:46.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap414:46.2_0": "…pe…",
  "tha-ap414:46.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap414:47.1_0": "Paṭisambhidā catasso,",
  "tha-ap414:47.2_0": "…pe…",
  "tha-ap414:47.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap414:48.1_0": "Itthaṁ sudaṁ āyasmā ekachattiyo thero imā gāthāyo abhāsitthāti;",
  "tha-ap414:49.1_0": "Ekachattiyattherassāpadānaṁ dutiyaṁ."
}