{
  "tha-ap31:0.1_0": "Therāpadāna",
  "tha-ap31:0.2_0": "Subhūtivagga",
  "tha-ap31:0.3_0": "9. Ekañjalikattheraapadāna",
  "tha-ap31:1.1_0": "“Suvaṇṇavaṇṇaṁ sambuddhaṁ,",
  "tha-ap31:1.2_0": "Gacchantaṁ antarāpaṇe;",
  "tha-ap31:1.3_0": "Vipassiṁ satthavāhaggaṁ,",
  "tha-ap31:1.4_0": "Naravaraṁ vināyakaṁ.",
  "tha-ap31:2.1_0": "Adantadamanaṁ tādiṁ,",
  "tha-ap31:2.2_0": "mahāvādiṁ mahāmatiṁ;",
  "tha-ap31:2.3_0": "Disvā pasanno sumano,",
  "tha-ap31:2.4_0": "ekañjalimakāsahaṁ.",
  "tha-ap31:3.1_0": "Ekanavutito kappe,",
  "tha-ap31:3.2_0": "yamañjaliṁ kariṁ tadā;",
  "tha-ap31:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap31:3.4_0": "añjalissa idaṁ phalaṁ.",
  "tha-ap31:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap31:4.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap31:4.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap31:4.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap31:5.1_0": "Itthaṁ sudaṁ āyasmā ekañjaliko thero imā gāthāyo abhāsitthāti.",
  "tha-ap31:6.1_0": "Ekañjalikattherassāpadānaṁ navamaṁ."
}