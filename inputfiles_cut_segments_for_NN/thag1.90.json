{
  "thag1.90:0.1_0": "Theragāthā",
  "thag1.90:0.2_0": "Ekakanipāta",
  "thag1.90:0.3_0": "Navamavagga",
  "thag1.90:0.4_0": "1.90. Sāmidattattheragāthā",
  "thag1.90:1.1_0": "“Pañcakkhandhā pariññātā,",
  "thag1.90:1.2_0": "tiṭṭhanti chinnamūlakā;",
  "thag1.90:1.3_0": "Vikkhīṇo jātisaṁsāro,",
  "thag1.90:1.4_0": "natthi dāni punabbhavo”ti.",
  "thag1.90:1.5_0": "…",
  "thag1.90:1.6_0": "Sāmidatto thero ….",
  "thag1.90:1.7_0": "vaggo navamo.",
  "thag1.90:2.0_0": "Tassuddānaṁ",
  "thag1.90:2.1_0": "Thero samitigutto ca,",
  "thag1.90:2.2_0": "kassapo sīhasavhayo;",
  "thag1.90:2.3_0": "Nīto sunāgo nāgito,",
  "thag1.90:2.4_0": "paviṭṭho ajjuno isi;",
  "thag1.90:2.5_0": "Devasabho ca yo thero,",
  "thag1.90:2.6_0": "sāmidatto mahabbaloti."
}