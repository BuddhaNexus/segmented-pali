{
  "tha-ap68:0.1_0": "Therāpadāna",
  "tha-ap68:0.2_0": "Sakacintaniyavagga",
  "tha-ap68:0.3_0": "6. Sucintitattheraapadāna",
  "tha-ap68:1.1_0": "“Giriduggacaro āsiṁ,",
  "tha-ap68:1.2_0": "abhijātova kesarī;",
  "tha-ap68:1.3_0": "Migasaṅghaṁ vadhitvāna,",
  "tha-ap68:1.4_0": "jīvāmi pabbatantare.",
  "tha-ap68:2.1_0": "Atthadassī tu bhagavā,",
  "tha-ap68:2.2_0": "sabbaññū vadataṁ varo;",
  "tha-ap68:2.3_0": "Mamuddharitukāmo so,",
  "tha-ap68:2.4_0": "āgacchi pabbatuttamaṁ.",
  "tha-ap68:3.1_0": "Pasadañca migaṁ hantvā,",
  "tha-ap68:3.2_0": "bhakkhituṁ samupāgamiṁ;",
  "tha-ap68:3.3_0": "Bhagavā tamhi samaye,",
  "tha-ap68:3.4_0": "bhikkhamāno upāgami.",
  "tha-ap68:4.1_0": "Varamaṁsāni paggayha,",
  "tha-ap68:4.2_0": "adāsiṁ tassa satthuno;",
  "tha-ap68:4.3_0": "Anumodi mahāvīro,",
  "tha-ap68:4.4_0": "nibbāpento mamaṁ tadā.",
  "tha-ap68:5.1_0": "Tena cittappasādena,",
  "tha-ap68:5.2_0": "giriduggaṁ pavisiṁ ahaṁ;",
  "tha-ap68:5.3_0": "Pītiṁ uppādayitvāna,",
  "tha-ap68:5.4_0": "tattha kālaṅkato ahaṁ.",
  "tha-ap68:6.1_0": "Etena maṁsadānena,",
  "tha-ap68:6.2_0": "cittassa paṇidhīhi ca;",
  "tha-ap68:6.3_0": "Pannarase kappasate,",
  "tha-ap68:6.4_0": "devaloke ramiṁ ahaṁ.",
  "tha-ap68:7.1_0": "Avasesesu kappesu,",
  "tha-ap68:7.2_0": "kusalaṁ cintitaṁ mayā;",
  "tha-ap68:7.3_0": "Teneva maṁsadānena,",
  "tha-ap68:7.4_0": "buddhānussaraṇena ca.",
  "tha-ap68:8.1_0": "Aṭṭhattiṁsamhi kappamhi,",
  "tha-ap68:8.2_0": "aṭṭha dīghāyunāmakā;",
  "tha-ap68:8.3_0": "Saṭṭhimhito kappasate,",
  "tha-ap68:8.4_0": "duve varuṇanāmakā.",
  "tha-ap68:9.1_0": "Paṭisambhidā catasso,",
  "tha-ap68:9.2_0": "…pe…",
  "tha-ap68:9.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap68:10.1_0": "Itthaṁ sudaṁ āyasmā sucintito thero imā gāthāyo abhāsitthāti.",
  "tha-ap68:11.1_0": "Sucintitattherassāpadānaṁ chaṭṭhaṁ."
}