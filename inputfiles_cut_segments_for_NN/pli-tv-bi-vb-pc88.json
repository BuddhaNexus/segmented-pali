{
  "pli-tv-bi-vb-pc88:0.1_0": "Theravāda Vinaya",
  "pli-tv-bi-vb-pc88:0.2_0": "Bhikkhunivibhaṅga",
  "pli-tv-bi-vb-pc88:0.3_0": "Pācittiyakaṇḍa",
  "pli-tv-bi-vb-pc88:0.4_0": "Chattupāhanavagga",
  "pli-tv-bi-vb-pc88:0.5_0": "88. Gandhavaṇṇakasikkhāpada",
  "pli-tv-bi-vb-pc88:1.1_0": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bi-vb-pc88:1.2_0": "Tena kho pana samayena chabbaggiyā bhikkhuniyo gandhavaṇṇakena nahāyanti.",
  "pli-tv-bi-vb-pc88:1.3_0": "Manussā ujjhāyanti khiyyanti vipācenti—",
  "pli-tv-bi-vb-pc88:1.4_0": "“kathañhi nāma bhikkhuniyo gandhavaṇṇakena nahāyissanti,",
  "pli-tv-bi-vb-pc88:1.4_1": "seyyathāpi gihiniyo kāmabhoginiyo”ti.",
  "pli-tv-bi-vb-pc88:1.5_0": "Assosuṁ kho bhikkhuniyo tesaṁ manussānaṁ ujjhāyantānaṁ khiyyantānaṁ vipācentānaṁ.",
  "pli-tv-bi-vb-pc88:1.6_0": "Yā tā bhikkhuniyo appicchā …pe… tā ujjhāyanti khiyyanti vipācenti—",
  "pli-tv-bi-vb-pc88:1.7_0": "“kathañhi nāma chabbaggiyā bhikkhuniyo gandhavaṇṇakena nahāyissantī”ti …pe…",
  "pli-tv-bi-vb-pc88:1.8_0": "“saccaṁ kira, bhikkhave,",
  "pli-tv-bi-vb-pc88:1.8_1": "chabbaggiyā bhikkhuniyo gandhavaṇṇakena nahāyantī”ti?",
  "pli-tv-bi-vb-pc88:1.9_0": "“Saccaṁ, bhagavā”ti.",
  "pli-tv-bi-vb-pc88:1.10_0": "Vigarahi buddho bhagavā …pe…",
  "pli-tv-bi-vb-pc88:1.11_0": "kathañhi nāma, bhikkhave,",
  "pli-tv-bi-vb-pc88:1.11_1": "chabbaggiyā bhikkhuniyo gandhavaṇṇakena nahāyissanti.",
  "pli-tv-bi-vb-pc88:1.12_0": "Netaṁ, bhikkhave, appasannānaṁ vā pasādāya …pe…",
  "pli-tv-bi-vb-pc88:1.13_0": "evañca pana, bhikkhave,",
  "pli-tv-bi-vb-pc88:1.13_1": "bhikkhuniyo imaṁ sikkhāpadaṁ uddisantu—",
  "pli-tv-bi-vb-pc88:1.14.1_0": "“Yā pana bhikkhunī gandhavaṇṇakena nahāyeyya, pācittiyan”ti.",
  "pli-tv-bi-vb-pc88:2.1.1_0": "Yā panāti",
  "pli-tv-bi-vb-pc88:2.1.2_0": "yā yādisā …pe…",
  "pli-tv-bi-vb-pc88:2.1.3_0": "bhikkhunīti",
  "pli-tv-bi-vb-pc88:2.1.4_0": "…pe… ayaṁ imasmiṁ atthe adhippetā bhikkhunīti.",
  "pli-tv-bi-vb-pc88:2.1.5_0": "Gandho nāma",
  "pli-tv-bi-vb-pc88:2.1.6_0": "yo koci gandho.",
  "pli-tv-bi-vb-pc88:2.1.7_0": "Vaṇṇakaṁ nāma",
  "pli-tv-bi-vb-pc88:2.1.8_0": "yaṁ kiñci vaṇṇakaṁ.",
  "pli-tv-bi-vb-pc88:2.1.9_0": "Nahāyeyyāti",
  "pli-tv-bi-vb-pc88:2.1.10_0": "nahāyati. Payoge dukkaṭaṁ,",
  "pli-tv-bi-vb-pc88:2.1.10_1": "nahānapariyosāne āpatti pācittiyassa.",
  "pli-tv-bi-vb-pc88:2.2.1_0": "Anāpatti—",
  "pli-tv-bi-vb-pc88:2.2.2_0": "ābādhappaccayā,",
  "pli-tv-bi-vb-pc88:2.2.3_0": "ummattikāya,",
  "pli-tv-bi-vb-pc88:2.2.4_0": "ādikammikāyāti.",
  "pli-tv-bi-vb-pc88:2.2.5_0": "Pañcamasikkhāpadaṁ niṭṭhitaṁ."
}