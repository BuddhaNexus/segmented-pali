{
  "thag1.50:0.1_0": "Theragāthā",
  "thag1.50:0.2_0": "Ekakanipāta",
  "thag1.50:0.3_0": "Pañcamavagga",
  "thag1.50:0.4_0": "1.50. Vimalattheragāthā",
  "thag1.50:1.1_0": "“Dharaṇī ca siñcati vāti,",
  "thag1.50:1.2_0": "Māluto vijjutā carati nabhe;",
  "thag1.50:1.3_0": "Upasamanti vitakkā,",
  "thag1.50:1.4_0": "Cittaṁ susamāhitaṁ mamā”ti.",
  "thag1.50:1.5_0": "…",
  "thag1.50:1.6_0": "Vimalo thero ….",
  "thag1.50:1.7_0": "vaggo pañcamo.",
  "thag1.50:2.0_0": "Tassuddānaṁ",
  "thag1.50:2.1_0": "Sirīvaḍḍho revato thero,",
  "thag1.50:2.2_0": "sumaṅgalo sānusavhayo;",
  "thag1.50:2.3_0": "Ramaṇīyavihārī ca,",
  "thag1.50:2.4_0": "samiddhiujjayasañjayā;",
  "thag1.50:2.5_0": "Rāmaṇeyyo ca so thero,",
  "thag1.50:2.6_0": "vimalo ca raṇañjahoti."
}