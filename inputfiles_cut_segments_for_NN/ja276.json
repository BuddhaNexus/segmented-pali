{
  "ja276:0.1_0": "Jātaka",
  "ja276:0.2_0": "Tikanipāta",
  "ja276:0.3_0": "Udapānavagga",
  "ja276:0.4_0": "6. Garudhammajātaka",
  "ja276:1.1_0": "“Tava saddhañca sīlañca,",
  "ja276:1.2_0": "viditvāna janādhipa;",
  "ja276:1.3_0": "Vaṇṇaṁ añjanavaṇṇena,",
  "ja276:1.4_0": "kāliṅgasmiṁ nimimhase”.",
  "ja276:2.1_0": "“Annabhaccā cabhaccā ca,",
  "ja276:2.2_0": "yodha uddissa gacchati;",
  "ja276:2.3_0": "Sabbe te appaṭikkhippā,",
  "ja276:2.4_0": "pubbācariyavaco idaṁ.",
  "ja276:3.1_0": "Dadāmi vo brāhmaṇā nāgametaṁ,",
  "ja276:3.2_0": "Rājārahaṁ rājabhoggaṁ yasassinaṁ;",
  "ja276:3.3_0": "Alaṅkataṁ hemajālābhichannaṁ,",
  "ja276:3.4_0": "Sasārathiṁ gacchatha yena kāman”ti.",
  "ja276:4.1_0": "Garudhammajātakaṁ chaṭṭhaṁ."
}