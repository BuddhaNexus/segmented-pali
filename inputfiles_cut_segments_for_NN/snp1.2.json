{
  "snp1.2:0.1_0": "Sutta Nipāta 1.2",
  "snp1.2:0.2_0": "Dhaniyasutta",
  "snp1.2:1.1_0": "“Pakkodano duddhakhīrohamasmi,",
  "snp1.2:1.2_0": "(iti dhaniyo gopo)",
  "snp1.2:1.3_0": "Anutīre mahiyā samānavāso;",
  "snp1.2:1.4_0": "Channā kuṭi āhito gini,",
  "snp1.2:1.5_0": "Atha ce patthayasī pavassa deva”.",
  "snp1.2:2.1_0": "“Akkodhano vigatakhilohamasmi,",
  "snp1.2:2.2_0": "(iti bhagavā)",
  "snp1.2:2.3_0": "Anutīre mahiyekarattivāso;",
  "snp1.2:2.4_0": "Vivaṭā kuṭi nibbuto gini,",
  "snp1.2:2.5_0": "Atha ce patthayasī pavassa deva”.",
  "snp1.2:3.1_0": "“Andhakamakasā na vijjare,",
  "snp1.2:3.2_0": "(iti dhaniyo gopo)",
  "snp1.2:3.3_0": "Kacche rūḷhatiṇe caranti gāvo;",
  "snp1.2:3.4_0": "Vuṭṭhimpi saheyyumāgataṁ,",
  "snp1.2:3.5_0": "Atha ce patthayasī pavassa deva”.",
  "snp1.2:4.1_0": "“Baddhāsi bhisī susaṅkhatā,",
  "snp1.2:4.2_0": "(iti bhagavā)",
  "snp1.2:4.3_0": "Tiṇṇo pāragato vineyya oghaṁ;",
  "snp1.2:4.4_0": "Attho bhisiyā na vijjati,",
  "snp1.2:4.5_0": "Atha ce patthayasī pavassa deva”.",
  "snp1.2:5.1_0": "“Gopī mama assavā alolā,",
  "snp1.2:5.2_0": "(iti dhaniyo gopo)",
  "snp1.2:5.3_0": "Dīgharattaṁ saṁvāsiyā manāpā;",
  "snp1.2:5.4_0": "Tassā na suṇāmi kiñci pāpaṁ,",
  "snp1.2:5.5_0": "Atha ce patthayasī pavassa deva”.",
  "snp1.2:6.1_0": "“Cittaṁ mama assavaṁ vimuttaṁ,",
  "snp1.2:6.2_0": "(iti bhagavā)",
  "snp1.2:6.3_0": "Dīgharattaṁ paribhāvitaṁ sudantaṁ;",
  "snp1.2:6.4_0": "Pāpaṁ pana me na vijjati,",
  "snp1.2:6.5_0": "Atha ce patthayasī pavassa deva”.",
  "snp1.2:7.1_0": "“Attavetanabhatohamasmi,",
  "snp1.2:7.2_0": "(iti dhaniyo gopo)",
  "snp1.2:7.3_0": "Puttā ca me samāniyā arogā;",
  "snp1.2:7.4_0": "Tesaṁ na suṇāmi kiñci pāpaṁ,",
  "snp1.2:7.5_0": "Atha ce patthayasī pavassa deva”.",
  "snp1.2:8.1_0": "“Nāhaṁ bhatakosmi kassaci,",
  "snp1.2:8.2_0": "(iti bhagavā)",
  "snp1.2:8.3_0": "Nibbiṭṭhena carāmi sabbaloke;",
  "snp1.2:8.4_0": "Attho bhatiyā na vijjati,",
  "snp1.2:8.5_0": "Atha ce patthayasī pavassa deva”.",
  "snp1.2:9.1_0": "“Atthi vasā atthi dhenupā,",
  "snp1.2:9.2_0": "(iti dhaniyo gopo)",
  "snp1.2:9.3_0": "Godharaṇiyo paveṇiyopi atthi;",
  "snp1.2:9.4_0": "Usabhopi gavampatīdha atthi,",
  "snp1.2:9.5_0": "Atha ce patthayasī pavassa deva”.",
  "snp1.2:10.1_0": "“Natthi vasā natthi dhenupā,",
  "snp1.2:10.2_0": "(iti bhagavā)",
  "snp1.2:10.3_0": "Godharaṇiyo paveṇiyopi natthi;",
  "snp1.2:10.4_0": "Usabhopi gavampatīdha natthi,",
  "snp1.2:10.5_0": "Atha ce patthayasī pavassa deva”.",
  "snp1.2:11.1_0": "“Khilā nikhātā asampavedhī,",
  "snp1.2:11.2_0": "(iti dhaniyo gopo)",
  "snp1.2:11.3_0": "Dāmā muñjamayā navā susaṇṭhānā;",
  "snp1.2:11.4_0": "Na hi sakkhinti dhenupāpi chettuṁ,",
  "snp1.2:11.5_0": "Atha ce patthayasī pavassa deva”.",
  "snp1.2:12.1_0": "“Usabhoriva chetva bandhanāni,",
  "snp1.2:12.2_0": "(iti bhagavā)",
  "snp1.2:12.3_0": "Nāgo pūtilataṁva dālayitvā;",
  "snp1.2:12.4_0": "Nāhaṁ punupessaṁ gabbhaseyyaṁ,",
  "snp1.2:12.5_0": "Atha ce patthayasī pavassa deva”.",
  "snp1.2:13.1_0": "Ninnañca thalañca pūrayanto,",
  "snp1.2:13.2_0": "Mahāmegho pavassi tāvadeva;",
  "snp1.2:13.3_0": "Sutvā devassa vassato,",
  "snp1.2:13.4_0": "Imamatthaṁ dhaniyo abhāsatha.",
  "snp1.2:14.1_0": "“Lābhā vata no anappakā,",
  "snp1.2:14.2_0": "Ye mayaṁ bhagavantaṁ addasāma;",
  "snp1.2:14.3_0": "Saraṇaṁ taṁ upema cakkhuma,",
  "snp1.2:14.4_0": "Satthā no hohi tuvaṁ mahāmuni.",
  "snp1.2:15.1_0": "Gopī ca ahañca assavā,",
  "snp1.2:15.2_0": "Brahmacariyaṁ sugate carāmase;",
  "snp1.2:15.3_0": "Jātimaraṇassa pāragū,",
  "snp1.2:15.4_0": "Dukkhassantakarā bhavāmase”.",
  "snp1.2:16.1_0": "“Nandati puttehi puttimā,",
  "snp1.2:16.2_0": "(iti māro pāpimā)",
  "snp1.2:16.3_0": "Gomā gohi tatheva nandati;",
  "snp1.2:16.4_0": "Upadhī hi narassa nandanā,",
  "snp1.2:16.5_0": "Na hi so nandati yo nirūpadhi”.",
  "snp1.2:17.1_0": "“Socati puttehi puttimā,",
  "snp1.2:17.2_0": "(iti bhagavā)",
  "snp1.2:17.3_0": "Gomā gohi tatheva socati;",
  "snp1.2:17.4_0": "Upadhī hi narassa socanā,",
  "snp1.2:17.5_0": "Na hi so socati yo nirūpadhī”ti.",
  "snp1.2:17.6_0": "Dhaniyasuttaṁ dutiyaṁ."
}