{
  "iti96:0.1_0": "Itivuttaka 96",
  "iti96:0.2_0": "Tikanipāta",
  "iti96:0.3_0": "Pañcamavagga",
  "iti96:0.4_0": "Kāmayogasutta",
  "iti96:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti96:2.1_0": "“Kāmayogayutto, bhikkhave, bhavayogayutto āgāmī hoti āgantā itthattaṁ.",
  "iti96:2.2_0": "Kāmayogavisaṁyutto, bhikkhave, bhavayogayutto anāgāmī hoti anāgantā itthattaṁ.",
  "iti96:2.3_0": "Kāmayogavisaṁyutto, bhikkhave, bhavayogavisaṁyutto arahā hoti, khīṇāsavo”ti.",
  "iti96:2.4_0": "Etamatthaṁ bhagavā avoca.",
  "iti96:2.5_0": "Tatthetaṁ iti vuccati:",
  "iti96:3.1_0": "“Kāmayogena saṁyuttā,",
  "iti96:3.2_0": "bhavayogena cūbhayaṁ;",
  "iti96:3.3_0": "Sattā gacchanti saṁsāraṁ,",
  "iti96:3.4_0": "jātimaraṇagāmino.",
  "iti96:4.1_0": "Ye ca kāme pahantvāna,",
  "iti96:4.2_0": "appattā āsavakkhayaṁ;",
  "iti96:4.3_0": "Bhavayogena saṁyuttā,",
  "iti96:4.4_0": "anāgāmīti vuccare.",
  "iti96:5.1_0": "Ye ca kho chinnasaṁsayā,",
  "iti96:5.2_0": "khīṇamānapunabbhavā;",
  "iti96:5.3_0": "Te ve pāraṅgatā loke,",
  "iti96:5.4_0": "ye pattā āsavakkhayan”ti.",
  "iti96:6.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti96:6.2_0": "Sattamaṁ.",
  "iti96:6.3_0": "Tatiyabhāṇavāraṁ."
}