{
  "mil3.6.7:0.1_0": "Milindapañha",
  "mil3.6.7:0.2_0": "Sativagga",
  "mil3.6.7:0.3_0": "7. Sarāgavītarāganānākaraṇapañha",
  "mil3.6.7:1.1_0": "Rājā āha—",
  "mil3.6.7:1.2_0": "“bhante nāgasena, kiṁ nānākaraṇaṁ sarāgassa ca vītarāgassa cā”ti?",
  "mil3.6.7:1.3_0": "“Eko kho, mahārāja, ajjhosito, eko anajjhosito”ti.",
  "mil3.6.7:1.4_0": "“Kiṁ etaṁ, bhante, ajjhosito anajjhosito nāmā”ti?",
  "mil3.6.7:1.5_0": "“Eko kho, mahārāja, atthiko, eko anatthiko”ti.",
  "mil3.6.7:1.6_0": "“Passāmahaṁ, bhante,",
  "mil3.6.7:1.6_1": "evarūpaṁ yo ca sarāgo,",
  "mil3.6.7:1.6_2": "yo ca vītarāgo,",
  "mil3.6.7:1.6_3": "sabbopeso sobhanaṁyeva icchati khādanīyaṁ vā bhojanīyaṁ vā,",
  "mil3.6.7:1.6_4": "na koci pāpakaṁ icchatī”ti.",
  "mil3.6.7:1.7_0": "“Avītarāgo kho, mahārāja,",
  "mil3.6.7:1.7_1": "rasapaṭisaṁvedī ca rasarāgapaṭisaṁvedī ca bhojanaṁ bhuñjati,",
  "mil3.6.7:1.7_2": "vītarāgo pana rasapaṭisaṁvedī bhojanaṁ bhuñjati,",
  "mil3.6.7:1.7_3": "no ca kho rasarāgapaṭisaṁvedī”ti.",
  "mil3.6.7:2.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.6.7:3.1_0": "Sarāgavītarāganānākaraṇapañho sattamo."
}