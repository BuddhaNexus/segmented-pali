{
  "kv21.6:0.1_0": "Kathāvatthu",
  "kv21.6:0.2_0": "Mahāpaṇṇāsaka",
  "kv21.6:0.3_0": "Ekavīsatimavagga",
  "kv21.6:0.4_0": "Sabbadisākathā",
  "kv21.6:1.1_0": "Sabbā disā buddhā tiṭṭhantīti?",
  "kv21.6:1.2_0": "Āmantā.",
  "kv21.6:1.3_0": "Puratthimāya disāya buddho tiṭṭhatīti?",
  "kv21.6:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv21.6:1.5_0": "puratthimāya disāya buddho tiṭṭhatīti?",
  "kv21.6:1.6_0": "Āmantā.",
  "kv21.6:1.7_0": "Kiṁnāmo so bhagavā, kiṁjacco, kiṅgotto,",
  "kv21.6:1.7_1": "kiṁnāmā tassa bhagavato mātāpitaro,",
  "kv21.6:1.7_2": "kiṁnāmaṁ tassa bhagavato sāvakayugaṁ,",
  "kv21.6:1.7_3": "konāmo tassa bhagavato upaṭṭhāko, kīdisaṁ cīvaraṁ dhāreti, kīdisaṁ pattaṁ dhāreti,",
  "kv21.6:1.7_4": "katarasmiṁ gāme vā nigame vā nagare vā raṭṭhe vā janapade vāti?",
  "kv21.6:1.8_0": "Na hevaṁ vattabbe …pe….",
  "kv21.6:2.1_0": "Dakkhiṇāya disāya …pe…",
  "kv21.6:2.2_0": "pacchimāya disāya …pe…",
  "kv21.6:2.3_0": "uttarāya disāya …pe…",
  "kv21.6:2.4_0": "heṭṭhimāya disāya buddho tiṭṭhatīti?",
  "kv21.6:2.5_0": "Na hevaṁ vattabbe …pe…",
  "kv21.6:2.6_0": "heṭṭhimāya disāya buddho tiṭṭhatīti?",
  "kv21.6:2.7_0": "Āmantā.",
  "kv21.6:2.8_0": "Kiṁnāmo so bhagavā …pe…",
  "kv21.6:2.9_0": "janapade vāti?",
  "kv21.6:2.10_0": "Na hevaṁ vattabbe …pe….",
  "kv21.6:3.1_0": "Uparimāya disāya buddho tiṭṭhatīti?",
  "kv21.6:3.2_0": "Na hevaṁ vattabbe …pe….",
  "kv21.6:3.3_0": "Uparimāya disāya buddho tiṭṭhatīti?",
  "kv21.6:3.4_0": "Āmantā.",
  "kv21.6:3.5_0": "Cātumahārājike tiṭṭhati …pe…",
  "kv21.6:3.6_0": "tāvatiṁse tiṭṭhati …pe…",
  "kv21.6:3.7_0": "yāme tiṭṭhati …pe…",
  "kv21.6:3.8_0": "tusite tiṭṭhati …pe…",
  "kv21.6:3.9_0": "nimmānaratiyā tiṭṭhati …pe…",
  "kv21.6:3.10_0": "paranimmitavasavattiyā tiṭṭhati …pe…",
  "kv21.6:3.11_0": "brahmaloke tiṭṭhatīti?",
  "kv21.6:3.12_0": "Na hevaṁ vattabbe …pe….",
  "kv21.6:3.13_0": "Sabbadisākathā niṭṭhitā."
}