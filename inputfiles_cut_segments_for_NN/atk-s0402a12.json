{
  "atk-s0402a12:0_0": "(13) 3. Dānavaggavaṇṇanā",
  "atk-s0402a12:1_0": "142.",
  "atk-s0402a12:1_1": "Tatiyassa paṭhame dānānīti diyyanakavasena dānāni, deyyadhammassetaṁ nāmaṁ. Savatthukā vā cetanā dānaṁ,",
  "atk-s0402a12:1_2": "sampattipariccāgassetaṁ nāmaṁ.",
  "atk-s0402a12:1_3": "Āmisadānanti cattāro paccayā diyyanakavasena āmisadānaṁ nāma.",
  "atk-s0402a12:1_4": "Dhammadānanti idhekacco amatapattipaṭipadaṁ kathetvā deti, idaṁ dhammadānaṁ nāma.",
  "atk-s0402a12:2_0": "143.",
  "atk-s0402a12:2_1": "Dutiye cattāro paccayā yajanakavasena yāgo nāma dhammopi yajanakavasena yāgoti veditabbo.",
  "atk-s0402a12:3_0": "144.",
  "atk-s0402a12:3_1": "Tatiye āmisassa cajanaṁ āmisacāgo, dhammassa cajanaṁ dhammacāgo.",
  "atk-s0402a12:3_2": "Catutthe upasaggamattaṁ viseso.",
  "atk-s0402a12:4_0": "146.",
  "atk-s0402a12:4_1": "Pañcame catunnaṁ paccayānaṁ bhuñjanaṁ āmisabhogo,",
  "atk-s0402a12:4_2": "dhammassa bhuñjanaṁ dhammabhogo.",
  "atk-s0402a12:4_3": "Chaṭṭhe upasaggamattaṁ viseso.",
  "atk-s0402a12:5_0": "148.",
  "atk-s0402a12:5_1": "Sattame catunnaṁ paccayānaṁ saṁvibhajanaṁ āmisasaṁvibhāgo,",
  "atk-s0402a12:5_2": "dhammassa saṁvibhajanaṁ dhammasaṁvibhāgo.",
  "atk-s0402a12:6_0": "149.",
  "atk-s0402a12:6_1": "Aṭṭhame catūhi paccayehi saṅgaho āmisasaṅgaho,",
  "atk-s0402a12:6_2": "dhammena saṅgaho dhammasaṅgaho.",
  "atk-s0402a12:7_0": "150.",
  "atk-s0402a12:7_1": "Navame catūhi paccayehi anuggaṇhanaṁ āmisānuggaho,",
  "atk-s0402a12:7_2": "dhammena anuggaṇhanaṁ dhammānuggaho.",
  "atk-s0402a12:8_0": "151.",
  "atk-s0402a12:8_1": "Dasame catūhi paccayehi anukampanaṁ āmisānukampā,",
  "atk-s0402a12:8_2": "dhammena anukampanaṁ dhammānukampāti.",
  "atk-s0402a12:9_0": "Dānavaggo tatiyo."
}