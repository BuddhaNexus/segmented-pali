{
  "kv22.3:0.1_0": "Kathāvatthu",
  "kv22.3:0.2_0": "Mahāpaṇṇāsaka",
  "kv22.3:0.3_0": "Bāvīsatimavagga",
  "kv22.3:0.4_0": "Āneñjakathā",
  "kv22.3:1.1_0": "Arahā āneñje ṭhito parinibbāyatīti?",
  "kv22.3:1.2_0": "Āmantā.",
  "kv22.3:1.3_0": "Nanu arahā pakaticitte ṭhito parinibbāyatīti?",
  "kv22.3:1.4_0": "Āmantā.",
  "kv22.3:1.5_0": "Hañci arahā pakaticitte ṭhito parinibbāyati, no ca vata re vattabbe—",
  "kv22.3:1.6_0": "“arahā āneñje ṭhito parinibbāyatī”ti.",
  "kv22.3:2.1_0": "Arahā āneñje ṭhito parinibbāyatīti?",
  "kv22.3:2.2_0": "Āmantā.",
  "kv22.3:2.3_0": "Arahā kiriyamaye citte ṭhito parinibbāyatīti?",
  "kv22.3:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv22.3:2.5_0": "nanu arahā vipākacitte ṭhito parinibbāyatīti?",
  "kv22.3:2.6_0": "Āmantā.",
  "kv22.3:2.7_0": "Hañci arahā vipākacitte ṭhito parinibbāyati, no ca vata re vattabbe—",
  "kv22.3:2.8_0": "“arahā āneñje ṭhito parinibbāyatī”ti.",
  "kv22.3:3.1_0": "Arahā āneñje ṭhito parinibbāyatīti?",
  "kv22.3:3.2_0": "Āmantā.",
  "kv22.3:3.3_0": "Arahā kiriyābyākate citte ṭhito parinibbāyatīti?",
  "kv22.3:3.4_0": "Na hevaṁ vattabbe …pe…",
  "kv22.3:3.5_0": "nanu arahā vipākābyākate citte ṭhito parinibbāyatīti?",
  "kv22.3:3.6_0": "Āmantā.",
  "kv22.3:3.7_0": "Hañci arahā vipākābyākate citte ṭhito parinibbāyati, no ca vata re vattabbe—",
  "kv22.3:3.8_0": "“arahā āneñje ṭhito parinibbāyatī”ti.",
  "kv22.3:4.1_0": "Arahā āneñje ṭhito parinibbāyatīti?",
  "kv22.3:4.2_0": "Āmantā.",
  "kv22.3:4.3_0": "Nanu bhagavā catutthajjhānā vuṭṭhahitvā samanantarā parinibbutoti?",
  "kv22.3:4.4_0": "Āmantā.",
  "kv22.3:4.5_0": "Hañci bhagavā catutthajjhānā vuṭṭhahitvā samanantarā parinibbuto, no ca vata re vattabbe—",
  "kv22.3:4.6_0": "“arahā āneñje ṭhito parinibbāyatī”ti.",
  "kv22.3:4.7_0": "Āneñjakathā niṭṭhitā."
}