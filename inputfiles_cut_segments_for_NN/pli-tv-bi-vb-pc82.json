{
  "pli-tv-bi-vb-pc82:0.1_0": "Theravāda Vinaya",
  "pli-tv-bi-vb-pc82:0.2_0": "Bhikkhunivibhaṅga",
  "pli-tv-bi-vb-pc82:0.3_0": "Pācittiyakaṇḍa",
  "pli-tv-bi-vb-pc82:0.4_0": "Kumāribhūtavagga",
  "pli-tv-bi-vb-pc82:0.5_0": "82. Anuvassasikkhāpada",
  "pli-tv-bi-vb-pc82:1.1_0": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bi-vb-pc82:1.2_0": "Tena kho pana samayena bhikkhuniyo anuvassaṁ vuṭṭhāpenti, upassayo na sammati.",
  "pli-tv-bi-vb-pc82:1.3_0": "Manussā ujjhāyanti khiyyanti vipācenti—",
  "pli-tv-bi-vb-pc82:1.4_0": "“kathañhi nāma bhikkhuniyo anuvassaṁ vuṭṭhāpessanti, upassayo na sammatī”ti.",
  "pli-tv-bi-vb-pc82:1.5_0": "Assosuṁ kho bhikkhuniyo tesaṁ manussānaṁ ujjhāyantānaṁ khiyyantānaṁ vipācentānaṁ.",
  "pli-tv-bi-vb-pc82:1.6_0": "Yā tā bhikkhuniyo appicchā …pe… tā ujjhāyanti khiyyanti vipācenti—",
  "pli-tv-bi-vb-pc82:1.7_0": "“kathañhi nāma bhikkhuniyo anuvassaṁ vuṭṭhāpessantī”ti …pe…",
  "pli-tv-bi-vb-pc82:1.8_0": "“saccaṁ kira, bhikkhave,",
  "pli-tv-bi-vb-pc82:1.8_1": "bhikkhuniyo anuvassaṁ vuṭṭhāpentī”ti?",
  "pli-tv-bi-vb-pc82:1.9_0": "“Saccaṁ, bhagavā”ti.",
  "pli-tv-bi-vb-pc82:1.10_0": "Vigarahi buddho bhagavā …pe…",
  "pli-tv-bi-vb-pc82:1.11_0": "kathañhi nāma, bhikkhave,",
  "pli-tv-bi-vb-pc82:1.11_1": "bhikkhuniyo anuvassaṁ vuṭṭhāpessanti.",
  "pli-tv-bi-vb-pc82:1.12_0": "Netaṁ, bhikkhave, appasannānaṁ vā pasādāya …pe…",
  "pli-tv-bi-vb-pc82:1.13_0": "evañca pana, bhikkhave,",
  "pli-tv-bi-vb-pc82:1.13_1": "bhikkhuniyo imaṁ sikkhāpadaṁ uddisantu—",
  "pli-tv-bi-vb-pc82:1.14.1_0": "“Yā pana bhikkhunī anuvassaṁ vuṭṭhāpeyya, pācittiyan”ti.",
  "pli-tv-bi-vb-pc82:2.1.1_0": "Yā panāti",
  "pli-tv-bi-vb-pc82:2.1.2_0": "yā yādisā …pe…",
  "pli-tv-bi-vb-pc82:2.1.3_0": "bhikkhunīti",
  "pli-tv-bi-vb-pc82:2.1.4_0": "…pe… ayaṁ imasmiṁ atthe adhippetā bhikkhunīti.",
  "pli-tv-bi-vb-pc82:2.1.5_0": "Anuvassanti",
  "pli-tv-bi-vb-pc82:2.1.6_0": "anusaṁvaccharaṁ.",
  "pli-tv-bi-vb-pc82:2.1.7_0": "Vuṭṭhāpeyyāti",
  "pli-tv-bi-vb-pc82:2.1.8_0": "upasampādeyya.",
  "pli-tv-bi-vb-pc82:2.1.9_0": "“Vuṭṭhāpessāmī”ti gaṇaṁ vā ācariniṁ vā pattaṁ vā cīvaraṁ vā pariyesati, sīmaṁ vā sammannati, āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc82:2.1.10_0": "Ñattiyā dukkaṭaṁ.",
  "pli-tv-bi-vb-pc82:2.1.11_0": "Dvīhi kammavācāhi dukkaṭā.",
  "pli-tv-bi-vb-pc82:2.1.12_0": "Kammavācāpariyosāne upajjhāyāya āpatti pācittiyassa.",
  "pli-tv-bi-vb-pc82:2.1.13_0": "Gaṇassa ca ācariniyā ca āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc82:2.2.1_0": "Anāpatti—",
  "pli-tv-bi-vb-pc82:2.2.2_0": "ekantarikaṁ vuṭṭhāpeti,",
  "pli-tv-bi-vb-pc82:2.2.3_0": "ummattikāya,",
  "pli-tv-bi-vb-pc82:2.2.4_0": "ādikammikāyāti.",
  "pli-tv-bi-vb-pc82:2.2.5_0": "Dvādasamasikkhāpadaṁ niṭṭhitaṁ."
}