{
  "kv11.2:0.1_0": "Kathāvatthu",
  "kv11.2:0.2_0": "Anusayapaṇṇāsaka",
  "kv11.2:0.3_0": "Ekādasamavagga",
  "kv11.2:0.4_0": "Ñāṇakathā",
  "kv11.2:1.1_0": "Aññāṇe vigate ñāṇavippayutte citte vattamāne na vattabbaṁ—",
  "kv11.2:1.2_0": "“ñāṇī”ti?",
  "kv11.2:1.3_0": "Āmantā.",
  "kv11.2:1.4_0": "Rāge vigate na vattabbaṁ—",
  "kv11.2:1.5_0": "“vītarāgo”ti?",
  "kv11.2:1.6_0": "Na hevaṁ vattabbe …pe…",
  "kv11.2:1.7_0": "aññāṇe vigate ñāṇavippayutte citte vattamāne na vattabbaṁ—",
  "kv11.2:1.8_0": "“ñāṇī”ti?",
  "kv11.2:1.9_0": "Āmantā.",
  "kv11.2:1.10_0": "Dose vigate …",
  "kv11.2:1.11_0": "mohe vigate …",
  "kv11.2:1.12_0": "kilese vigate na vattabbaṁ—",
  "kv11.2:1.13_0": "“nikkileso”ti?",
  "kv11.2:1.14_0": "Na hevaṁ vattabbe …pe….",
  "kv11.2:2.1_0": "Rāge vigate vattabbaṁ—",
  "kv11.2:2.2_0": "“vītarāgo”ti?",
  "kv11.2:2.3_0": "Āmantā.",
  "kv11.2:2.4_0": "Aññāṇe vigate ñāṇavippayutte citte vattamāne vattabbaṁ—",
  "kv11.2:2.5_0": "“ñāṇī”ti?",
  "kv11.2:2.6_0": "Na hevaṁ vattabbe …pe…",
  "kv11.2:2.7_0": "dose vigate …",
  "kv11.2:2.8_0": "mohe vigate …",
  "kv11.2:2.9_0": "kilese vigate vattabbaṁ—",
  "kv11.2:2.10_0": "“nikkileso”ti?",
  "kv11.2:2.11_0": "Āmantā.",
  "kv11.2:2.12_0": "Aññāṇe vigate ñāṇavippayutte citte vattamāne vattabbaṁ—",
  "kv11.2:2.13_0": "“ñāṇī”ti?",
  "kv11.2:2.14_0": "Na hevaṁ vattabbe …pe….",
  "kv11.2:3.1_0": "Aññāṇe vigate ñāṇavippayutte citte vattamāne vattabbaṁ—",
  "kv11.2:3.2_0": "“ñāṇī”ti?",
  "kv11.2:3.3_0": "Āmantā.",
  "kv11.2:3.4_0": "Atītena ñāṇena ñāṇī niruddhena vigatena paṭipassaddhena ñāṇena ñāṇīti?",
  "kv11.2:3.5_0": "Na hevaṁ vattabbe …pe….",
  "kv11.2:3.6_0": "Ñāṇakathā niṭṭhitā."
}