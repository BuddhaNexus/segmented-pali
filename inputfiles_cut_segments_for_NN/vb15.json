{
  "vb15:0.1_0": "Vibhaṅga",
  "vb15:0.2_0": "Paṭisambhidāvibhaṅga",
  "vb15:1.0.1_0": "Suttantabhājanīya",
  "vb15:1.0.2_0": "1. Saṅgahavāra",
  "vb15:1.1_0": "Catasso paṭisambhidā—",
  "vb15:1.2_0": "atthapaṭisambhidā, dhammapaṭisambhidā, niruttipaṭisambhidā, paṭibhānapaṭisambhidā.",
  "vb15:1.3_0": "Atthe ñāṇaṁ atthapaṭisambhidā,",
  "vb15:1.3_1": "dhamme ñāṇaṁ dhammapaṭisambhidā,",
  "vb15:1.3_2": "tatra dhammaniruttābhilāpe ñāṇaṁ niruttipaṭisambhidā,",
  "vb15:1.3_3": "ñāṇesu ñāṇaṁ paṭibhānapaṭisambhidā.",
  "vb15:1.4_0": "Ayaṁ saṅgahavāro.",
  "vb15:2.0_0": "2. Saccavāra",
  "vb15:2.1_0": "Catasso paṭisambhidā—",
  "vb15:2.2_0": "atthapaṭisambhidā, dhammapaṭisambhidā, niruttipaṭisambhidā, paṭibhānapaṭisambhidā.",
  "vb15:2.3_0": "Dukkhe ñāṇaṁ atthapaṭisambhidā,",
  "vb15:2.3_1": "dukkhasamudaye ñāṇaṁ dhammapaṭisambhidā,",
  "vb15:2.3_2": "dukkhanirodhe ñāṇaṁ atthapaṭisambhidā,",
  "vb15:2.3_3": "dukkhanirodhagāminiyā paṭipadāya ñāṇaṁ dhammapaṭisambhidā,",
  "vb15:2.3_4": "tatra dhammaniruttābhilāpe ñāṇaṁ niruttipaṭisambhidā,",
  "vb15:2.3_5": "ñāṇesu ñāṇaṁ paṭibhānapaṭisambhidā.",
  "vb15:2.4_0": "Ayaṁ saccavāro.",
  "vb15:3.0_0": "3. Hetuvāra",
  "vb15:3.1_0": "Catasso paṭisambhidā—",
  "vb15:3.2_0": "atthapaṭisambhidā, dhammapaṭisambhidā niruttipaṭisambhidā, paṭibhānapaṭisambhidā.",
  "vb15:3.3_0": "Hetumhi ñāṇaṁ dhammapaṭisambhidā,",
  "vb15:3.3_1": "hetuphale ñāṇaṁ atthapaṭisambhidā,",
  "vb15:3.3_2": "tatra dhammaniruttābhilāpe ñāṇaṁ niruttipaṭisambhidā,",
  "vb15:3.3_3": "ñāṇesu ñāṇaṁ paṭibhānapaṭisambhidā.",
  "vb15:3.4_0": "Ayaṁ hetuvāro.",
  "vb15:4.0_0": "4. Dhammavāra",
  "vb15:4.1_0": "Catasso paṭisambhidā—",
  "vb15:4.2_0": "atthapaṭisambhidā, dhammapaṭisambhidā, niruttipaṭisambhidā, paṭibhānapaṭisambhidā.",
  "vb15:4.3_0": "Ye dhammā jātā bhūtā sañjātā nibbattā abhinibbattā pātubhūtā,",
  "vb15:4.3_1": "imesu dhammesu ñāṇaṁ atthapaṭisambhidā;",
  "vb15:4.4_0": "yamhā dhammā te dhammā jātā bhūtā sañjātā nibbattā abhinibbattā pātubhūtā,",
  "vb15:4.4_1": "tesu dhammesu ñāṇaṁ dhammapaṭisambhidā,",
  "vb15:4.4_2": "tatra dhammaniruttābhilāpe ñāṇaṁ niruttipaṭisambhidā,",
  "vb15:4.4_3": "ñāṇesu ñāṇaṁ paṭibhānapaṭisambhidā.",
  "vb15:4.5_0": "Ayaṁ dhammavāro.",
  "vb15:5.0_0": "5. Paṭiccasamuppādavāra",
  "vb15:5.1_0": "Catasso paṭisambhidā—",
  "vb15:5.2_0": "atthapaṭisambhidā, dhammapaṭisambhidā, niruttipaṭisambhidā, paṭibhānapaṭisambhidā.",
  "vb15:5.3_0": "Jarāmaraṇe ñāṇaṁ atthapaṭisambhidā,",
  "vb15:5.3_1": "jarāmaraṇasamudaye ñāṇaṁ dhammapaṭisambhidā,",
  "vb15:5.3_2": "jarāmaraṇanirodhe ñāṇaṁ atthapaṭisambhidā,",
  "vb15:5.3_3": "jarāmaraṇanirodhagāminiyā paṭipadāya ñāṇaṁ dhammapaṭisambhidā,",
  "vb15:5.3_4": "tatra dhammaniruttābhilāpe ñāṇaṁ niruttipaṭisambhidā,",
  "vb15:5.3_5": "ñāṇesu ñāṇaṁ paṭibhānapaṭisambhidā.",
  "vb15:6.1_0": "Catasso paṭisambhidā—",
  "vb15:6.2_0": "atthapaṭisambhidā, dhammapaṭisambhidā, niruttipaṭisambhidā, paṭibhānapaṭisambhidā.",
  "vb15:6.3_0": "Jātiyā ñāṇaṁ …pe…",
  "vb15:6.4_0": "bhave ñāṇaṁ …pe…",
  "vb15:6.5_0": "upādāne ñāṇaṁ …pe…",
  "vb15:6.6_0": "taṇhāya ñāṇaṁ …pe…",
  "vb15:6.7_0": "vedanāya ñāṇaṁ …pe…",
  "vb15:6.8_0": "phasse ñāṇaṁ …pe…",
  "vb15:6.9_0": "saḷāyatane ñāṇaṁ …pe…",
  "vb15:6.10_0": "nāmarūpe ñāṇaṁ …pe…",
  "vb15:6.11_0": "viññāṇe ñāṇaṁ …pe…",
  "vb15:6.12_0": "saṅkhāresu ñāṇaṁ atthapaṭisambhidā,",
  "vb15:6.12_1": "saṅkhārasamudaye ñāṇaṁ dhammapaṭisambhidā,",
  "vb15:6.12_2": "saṅkhāranirodhe ñāṇaṁ atthapaṭisambhidā,",
  "vb15:6.12_3": "saṅkhāranirodhagāminiyā paṭipadāya ñāṇaṁ dhammapaṭisambhidā,",
  "vb15:6.12_4": "tatra dhammaniruttābhilāpe ñāṇaṁ niruttipaṭisambhidā,",
  "vb15:6.12_5": "ñāṇesu ñāṇaṁ paṭibhānapaṭisambhidā.",
  "vb15:6.13_0": "Ayaṁ paṭiccasamuppādavāro.",
  "vb15:7.0_0": "6. Pariyattivāra",
  "vb15:7.1_0": "Catasso paṭisambhidā—",
  "vb15:7.2_0": "atthapaṭisambhidā, dhammapaṭisambhidā, niruttipaṭisambhidā, paṭibhānapaṭisambhidā.",
  "vb15:8.1_0": "Tattha katamā dhammapaṭisambhidā?",
  "vb15:8.2_0": "Idha bhikkhu dhammaṁ jānāti—",
  "vb15:8.3_0": "suttaṁ geyyaṁ veyyākaraṇaṁ gāthaṁ udānaṁ itivuttakaṁ jātakaṁ abbhutadhammaṁ vedallaṁ.",
  "vb15:8.4_0": "Ayaṁ vuccati “dhammapaṭisambhidā”.",
  "vb15:8.5_0": "So tassa tasseva bhāsitassa atthaṁ jānāti—",
  "vb15:8.6_0": "“ayaṁ imassa bhāsitassa attho, ayaṁ imassa bhāsitassa attho”ti.",
  "vb15:8.7_0": "Ayaṁ vuccati “atthapaṭisambhidā”,",
  "vb15:8.7_1": "tatra dhammaniruttābhilāpe ñāṇaṁ niruttipaṭisambhidā,",
  "vb15:8.7_2": "ñāṇesu ñāṇaṁ paṭibhānapaṭisambhidā.",
  "vb15:8.8_0": "Ayaṁ pariyattivāro.",
  "vb15:9.1_0": "Suttantabhājanīyaṁ.",
  "vb15:10.0.1_0": "Abhidhammabhājanīya",
  "vb15:10.0.2_0": "1. Kusalavāra",
  "vb15:10.0.3_0": "1.1. Kāmāvacara",
  "vb15:10.1_0": "Catasso paṭisambhidā—",
  "vb15:10.2_0": "atthapaṭisambhidā, dhammapaṭisambhidā, niruttipaṭisambhidā, paṭibhānapaṭisambhidā.",
  "vb15:11.1_0": "Katame dhammā kusalā?",
  "vb15:11.2_0": "Yasmiṁ samaye kāmāvacaraṁ kusalaṁ cittaṁ uppannaṁ hoti somanassasahagataṁ ñāṇasampayuttaṁ rūpārammaṇaṁ vā …pe…",
  "vb15:11.3_0": "dhammārammaṇaṁ vā yaṁ yaṁ vā panārabbha, tasmiṁ samaye phasso hoti …pe… avikkhepo hoti.",
  "vb15:11.4_0": "Ime dhammā kusalā.",
  "vb15:11.5_0": "Imesu dhammesu ñāṇaṁ dhammapaṭisambhidā.",
  "vb15:11.6_0": "Tesaṁ vipāke ñāṇaṁ atthapaṭisambhidā,",
  "vb15:11.6_1": "yāya niruttiyā tesaṁ dhammānaṁ paññatti hoti,",
  "vb15:11.6_2": "tatra dhammaniruttābhilāpe ñāṇaṁ niruttipaṭisambhidā.",
  "vb15:11.7_0": "Yena ñāṇena tāni ñāṇāni jānāti—",
  "vb15:11.8_0": "“imāni ñāṇāni idamatthajotakānī”ti, ñāṇesu ñāṇaṁ paṭibhānapaṭisambhidā.",
  "vb15:12.1_0": "Catasso paṭisambhidā—",
  "vb15:12.2_0": "atthapaṭisambhidā, dhammapaṭisambhidā, niruttipaṭisambhidā, paṭibhānapaṭisambhidā.",
  "vb15:13.1_0": "Katame dhammā kusalā?",
  "vb15:13.2_0": "Yasmiṁ samaye kāmāvacaraṁ kusalaṁ cittaṁ uppannaṁ hoti somanassasahagataṁ ñāṇasampayuttaṁ sasaṅkhārena …pe…",
  "vb15:13.3_0": "somanassasahagataṁ ñāṇavippayuttaṁ …pe…",
  "vb15:13.4_0": "somanassasahagataṁ ñāṇavippayuttaṁ sasaṅkhārena …pe…",
  "vb15:13.5_0": "upekkhāsahagataṁ ñāṇasampayuttaṁ …pe…",
  "vb15:13.6_0": "upekkhāsahagataṁ ñāṇasampayuttaṁ sasaṅkhārena …pe…",
  "vb15:13.7_0": "upekkhāsahagataṁ ñāṇavippayuttaṁ …pe…",
  "vb15:13.8_0": "upekkhāsahagataṁ ñāṇavippayuttaṁ sasaṅkhārena rūpārammaṇaṁ vā …pe…",
  "vb15:13.9_0": "dhammārammaṇaṁ vā yaṁ yaṁ vā panārabbha, tasmiṁ samaye phasso hoti …pe… avikkhepo hoti.",
  "vb15:13.10_0": "Ime dhammā kusalā.",
  "vb15:13.11_0": "Imesu dhammesu ñāṇaṁ dhammapaṭisambhidā.",
  "vb15:13.12_0": "Tesaṁ vipāke ñāṇaṁ atthapaṭisambhidā.",
  "vb15:13.13_0": "Yāya niruttiyā tesaṁ dhammānaṁ paññatti hoti tatra dhammaniruttābhilāpe ñāṇaṁ niruttipaṭisambhidā.",
  "vb15:13.14_0": "Yena ñāṇena tāni ñāṇāni jānāti—",
  "vb15:13.15_0": "“imāni ñāṇāni idamatthajotakānī”ti, ñāṇesu ñāṇaṁ paṭibhānapaṭisambhidā.",
  "vb15:14.0_0": "1.2. Rūpāvacara",
  "vb15:14.1_0": "Catasso paṭisambhidā—",
  "vb15:14.2_0": "atthapaṭisambhidā, dhammapaṭisambhidā, niruttipaṭisambhidā, paṭibhānapaṭisambhidā.",
  "vb15:15.1_0": "Katame dhammā kusalā?",
  "vb15:15.2_0": "Yasmiṁ samaye rūpūpapattiyā maggaṁ bhāveti vivicceva kāmehi …pe…",
  "vb15:15.2_1": "paṭhamaṁ jhānaṁ upasampajja viharati pathavīkasiṇaṁ,",
  "vb15:15.2_2": "tasmiṁ samaye phasso hoti …pe…",
  "vb15:15.2_3": "avikkhepo hoti.",
  "vb15:15.3_0": "Ime dhammā kusalā.",
  "vb15:15.4_0": "Imesu dhammesu ñāṇaṁ dhammapaṭisambhidā.",
  "vb15:15.5_0": "Tesaṁ vipāke ñāṇaṁ atthapaṭisambhidā.",
  "vb15:15.6_0": "Yāya niruttiyā tesaṁ dhammānaṁ paññatti hoti tatra dhammaniruttābhilāpe ñāṇaṁ niruttipaṭisambhidā.",
  "vb15:15.7_0": "Yena ñāṇena tāni ñāṇāni jānāti—",
  "vb15:15.8_0": "“imāni ñāṇāni idamatthajotakānī”ti, ñāṇesu ñāṇaṁ paṭibhānapaṭisambhidā.",
  "vb15:16.0_0": "1.3. Arūpāvacara",
  "vb15:16.1_0": "Catasso paṭisambhidā—",
  "vb15:16.2_0": "atthapaṭisambhidā, dhammapaṭisambhidā, niruttipaṭisambhidā, paṭibhānapaṭisambhidā.",
  "vb15:17.1_0": "Katame dhammā kusalā?",
  "vb15:17.2_0": "Yasmiṁ samaye arūpūpapattiyā maggaṁ bhāveti sabbaso ākiñcaññāyatanaṁ samatikkamma nevasaññānāsaññāyatanasaññāsahagataṁ sukhassa ca pahānā …pe…",
  "vb15:17.2_1": "catutthaṁ jhānaṁ upasampajja viharati,",
  "vb15:17.2_2": "tasmiṁ samaye phasso hoti …pe…",
  "vb15:17.2_3": "avikkhepo hoti.",
  "vb15:17.3_0": "Ime dhammā kusalā.",
  "vb15:17.4_0": "Imesu dhammesu ñāṇaṁ dhammapaṭisambhidā.",
  "vb15:17.5_0": "Tesaṁ vipāke ñāṇaṁ atthapaṭisambhidā.",
  "vb15:17.6_0": "Yāya niruttiyā tesaṁ dhammānaṁ paññatti hoti tatra dhammaniruttābhilāpe ñāṇaṁ niruttipaṭisambhidā.",
  "vb15:17.7_0": "Yena ñāṇena tāni ñāṇāni jānāti—",
  "vb15:17.8_0": "“imāni ñāṇāni idamatthajotakānī”ti, ñāṇesu ñāṇaṁ paṭibhānapaṭisambhidā.",
  "vb15:18.0_0": "1.4. Lokuttara",
  "vb15:18.1_0": "Catasso paṭisambhidā—",
  "vb15:18.2_0": "atthapaṭisambhidā, dhammapaṭisambhidā, niruttipaṭisambhidā, paṭibhānapaṭisambhidā.",
  "vb15:19.1_0": "Katame dhammā kusalā?",
  "vb15:19.2_0": "Yasmiṁ samaye lokuttaraṁ jhānaṁ bhāveti niyyānikaṁ apacayagāmiṁ diṭṭhigatānaṁ pahānāya paṭhamāya bhūmiyā pattiyā vivicceva kāmehi …pe…",
  "vb15:19.2_1": "paṭhamaṁ jhānaṁ upasampajja viharati dukkhapaṭipadaṁ dandhābhiññaṁ,",
  "vb15:19.2_2": "tasmiṁ samaye phasso hoti …pe…",
  "vb15:19.2_3": "avikkhepo hoti.",
  "vb15:19.3_0": "Ime dhammā kusalā.",
  "vb15:19.4_0": "Imesu dhammesu ñāṇaṁ dhammapaṭisambhidā.",
  "vb15:19.5_0": "Tesaṁ vipāke ñāṇaṁ atthapaṭisambhidā.",
  "vb15:19.6_0": "Yāya niruttiyā tesaṁ dhammānaṁ paññatti hoti tatra dhammaniruttābhilāpe ñāṇaṁ niruttipaṭisambhidā.",
  "vb15:19.7_0": "Yena ñāṇena tāni ñāṇāni jānāti—",
  "vb15:19.8_0": "“imāni ñāṇāni idamatthajotakānī”ti, ñāṇesu ñāṇaṁ paṭibhānapaṭisambhidā.",
  "vb15:20.0_0": "2. Akusalavāra",
  "vb15:20.1_0": "Catasso paṭisambhidā—",
  "vb15:20.2_0": "atthapaṭisambhidā, dhammapaṭisambhidā, niruttipaṭisambhidā, paṭibhānapaṭisambhidā.",
  "vb15:21.1_0": "Katame dhammā akusalā?",
  "vb15:21.2_0": "Yasmiṁ samaye akusalaṁ cittaṁ uppannaṁ hoti somanassasahagataṁ diṭṭhigatasampayuttaṁ rūpārammaṇaṁ vā …pe…",
  "vb15:21.3_0": "dhammārammaṇaṁ vā yaṁ yaṁ vā panārabbha, tasmiṁ samaye phasso hoti …pe… avikkhepo hoti.",
  "vb15:21.4_0": "Ime dhammā akusalā.",
  "vb15:21.5_0": "Imesu dhammesu ñāṇaṁ dhammapaṭisambhidā.",
  "vb15:21.6_0": "Tesaṁ vipāke ñāṇaṁ atthapaṭisambhidā.",
  "vb15:21.7_0": "Yāya niruttiyā tesaṁ dhammānaṁ paññatti hoti tatra dhammaniruttābhilāpe ñāṇaṁ niruttipaṭisambhidā.",
  "vb15:21.8_0": "Yena ñāṇena tāni ñāṇāni jānāti—",
  "vb15:21.9_0": "“imāni ñāṇāni idamatthajotakānī”ti, ñāṇesu ñāṇaṁ paṭibhānapaṭisambhidā.",
  "vb15:22.1_0": "Catasso paṭisambhidā—",
  "vb15:22.2_0": "atthapaṭisambhidā, dhammapaṭisambhidā, niruttipaṭisambhidā, paṭibhānapaṭisambhidā.",
  "vb15:23.1_0": "Katame dhammā akusalā?",
  "vb15:23.2_0": "Yasmiṁ samaye akusalaṁ cittaṁ uppannaṁ hoti somanassasahagataṁ diṭṭhigatasampayuttaṁ sasaṅkhārena …pe…",
  "vb15:23.3_0": "somanassasahagataṁ diṭṭhigatavippayuttaṁ …pe…",
  "vb15:23.4_0": "somanassasahagataṁ diṭṭhigatavippayuttaṁ sasaṅkhārena …pe…",
  "vb15:23.5_0": "upekkhāsahagataṁ diṭṭhigatasampayuttaṁ …pe…",
  "vb15:23.6_0": "upekkhāsahagataṁ diṭṭhigatasampayuttaṁ sasaṅkhārena …pe…",
  "vb15:23.7_0": "upekkhāsahagataṁ diṭṭhigatavippayuttaṁ …pe…",
  "vb15:23.8_0": "upekkhāsahagataṁ diṭṭhigatavippayuttaṁ sasaṅkhārena …pe…",
  "vb15:23.9_0": "domanassasahagataṁ paṭighasampayuttaṁ …pe…",
  "vb15:23.10_0": "domanassasahagataṁ paṭighasampayuttaṁ sasaṅkhārena …pe…",
  "vb15:23.11_0": "upekkhāsahagataṁ vicikicchāsampayuttaṁ …pe…",
  "vb15:23.12_0": "upekkhāsahagataṁ uddhaccasampayuttaṁ rūpārammaṇaṁ vā …pe…",
  "vb15:23.13_0": "dhammārammaṇaṁ vā yaṁ yaṁ vā panārabbha, tasmiṁ samaye phasso hoti …pe… avikkhepo hoti.",
  "vb15:23.14_0": "Ime dhammā akusalā.",
  "vb15:23.15_0": "Imesu dhammesu ñāṇaṁ dhammapaṭisambhidā.",
  "vb15:23.16_0": "Tesaṁ vipāke ñāṇaṁ atthapaṭisambhidā.",
  "vb15:23.17_0": "Yāya niruttiyā tesaṁ dhammānaṁ paññatti hoti tatra dhammaniruttābhilāpe ñāṇaṁ niruttipaṭisambhidā.",
  "vb15:23.18_0": "Yena ñāṇena tāni ñāṇāni jānāti—",
  "vb15:23.19_0": "“imāni ñāṇāni idamatthajotakānī”ti, ñāṇesu ñāṇaṁ paṭibhānapaṭisambhidā.",
  "vb15:24.0.1_0": "3. Vipākavāra",
  "vb15:24.0.2_0": "3.1. Kusalavipāka",
  "vb15:24.0.3_0": "3.1.1. Ahetukakusala",
  "vb15:24.1_0": "Tisso paṭisambhidā—",
  "vb15:24.2_0": "atthapaṭisambhidā, niruttipaṭisambhidā, paṭibhānapaṭisambhidā.",
  "vb15:25.1_0": "Katame dhammā abyākatā?",
  "vb15:25.2_0": "Yasmiṁ samaye kāmāvacarassa kusalassa kammassa katattā upacitattā vipākaṁ cakkhuviññāṇaṁ uppannaṁ hoti upekkhāsahagataṁ rūpārammaṇaṁ,",
  "vb15:25.2_1": "tasmiṁ samaye phasso hoti,",
  "vb15:25.2_2": "vedanā hoti,",
  "vb15:25.2_3": "saññā hoti,",
  "vb15:25.2_4": "cetanā hoti,",
  "vb15:25.2_5": "cittaṁ hoti,",
