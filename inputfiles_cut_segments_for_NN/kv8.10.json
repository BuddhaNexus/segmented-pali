{
  "kv8.10:0.1_0": "Kathāvatthu",
  "kv8.10:0.2_0": "Mahāpaṇṇāsaka",
  "kv8.10:0.3_0": "Aṭṭhamavagga",
  "kv8.10:0.4_0": "Jīvitindriyakathā",
  "kv8.10:1.1_0": "Natthi rūpajīvitindriyanti?",
  "kv8.10:1.2_0": "Āmantā.",
  "kv8.10:1.3_0": "Natthi rūpīnaṁ dhammānaṁ āyu ṭhiti yapanā yāpanā iriyanā vattanā pālanāti?",
  "kv8.10:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv8.10:1.5_0": "atthi rūpīnaṁ dhammānaṁ āyu ṭhiti yapanā yāpanā iriyanā vattanā pālanāti?",
  "kv8.10:1.6_0": "Āmantā.",
  "kv8.10:1.7_0": "Hañci atthi rūpīnaṁ dhammānaṁ āyu ṭhiti yapanā yāpanā iriyanā vattanā pālanā,",
  "kv8.10:1.7_1": "no ca vata re vattabbe—",
  "kv8.10:1.8_0": "“natthi rūpajīvitindriyan”ti.",
  "kv8.10:2.1_0": "Atthi arūpīnaṁ dhammānaṁ āyu ṭhiti yapanā yāpanā iriyanā vattanā pālanā, atthi arūpajīvitindriyanti?",
  "kv8.10:2.2_0": "Āmantā.",
  "kv8.10:2.3_0": "Atthi rūpīnaṁ dhammānaṁ āyu ṭhiti yapanā yāpanā iriyanā vattanā pālanā, atthi rūpajīvitindriyanti?",
  "kv8.10:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv8.10:2.5_0": "atthi rūpīnaṁ dhammānaṁ āyu ṭhiti yapanā yāpanā iriyanā vattanā pālanā, natthi rūpajīvitindriyanti?",
  "kv8.10:2.6_0": "Āmantā.",
  "kv8.10:2.7_0": "Atthi arūpīnaṁ dhammānaṁ āyu ṭhiti yapanā yāpanā iriyanā vattanā pālanā,",
  "kv8.10:2.7_1": "natthi arūpajīvitindriyanti?",
  "kv8.10:2.8_0": "Na hevaṁ vattabbe …pe….",
  "kv8.10:3.1_0": "Arūpīnaṁ dhammānaṁ āyu arūpajīvitindriyanti?",
  "kv8.10:3.2_0": "Āmantā.",
  "kv8.10:3.3_0": "Rūpīnaṁ dhammānaṁ āyu rūpajīvitindriyanti?",
  "kv8.10:3.4_0": "Na hevaṁ vattabbe …pe…",
  "kv8.10:3.5_0": "rūpīnaṁ dhammānaṁ āyu na vattabbaṁ—",
  "kv8.10:3.6_0": "“rūpajīvitindriyan”ti?",
  "kv8.10:3.7_0": "Āmantā.",
  "kv8.10:3.8_0": "Arūpīnaṁ dhammānaṁ āyu na vattabbaṁ—",
  "kv8.10:3.9_0": "“arūpajīvitindriyan”ti?",
  "kv8.10:3.10_0": "Na hevaṁ vattabbe …pe….",
  "kv8.10:4.1_0": "Rūpīnaṁ dhammānaṁ āyu arūpajīvitindriyanti?",
  "kv8.10:4.2_0": "Āmantā.",
  "kv8.10:4.3_0": "Arūpīnaṁ dhammānaṁ āyu rūpajīvitindriyanti?",
  "kv8.10:4.4_0": "Na hevaṁ vattabbe …pe….",
  "kv8.10:5.1_0": "Arūpīnaṁ dhammānaṁ āyu na vattabbaṁ—",
  "kv8.10:5.2_0": "“rūpajīvitindriyan”ti?",
  "kv8.10:5.3_0": "Āmantā.",
  "kv8.10:5.4_0": "Rūpīnaṁ dhammānaṁ āyu na vattabbaṁ—",
  "kv8.10:5.5_0": "“arūpajīvitindriyan”ti?",
  "kv8.10:5.6_0": "Na hevaṁ vattabbe …pe….",
  "kv8.10:6.1_0": "Rūpīnañca arūpīnañca dhammānaṁ āyu arūpajīvitindriyanti?",
  "kv8.10:6.2_0": "Āmantā.",
  "kv8.10:6.3_0": "Rūpīnañca arūpīnañca dhammānaṁ āyu rūpajīvitindriyanti?",
  "kv8.10:6.4_0": "Na hevaṁ vattabbe …pe….",
  "kv8.10:7.1_0": "Rūpīnañca arūpīnañca dhammānaṁ āyu na vattabbaṁ—",
  "kv8.10:7.2_0": "“rūpajīvitindriyan”ti?",
  "kv8.10:7.3_0": "Āmantā.",
  "kv8.10:7.4_0": "Rūpīnañca arūpīnañca dhammānaṁ āyu na vattabbaṁ—",
  "kv8.10:7.5_0": "“arūpajīvitindriyan”ti?",
  "kv8.10:7.6_0": "Na hevaṁ vattabbe …pe…",
  "kv8.10:7.7_0": "natthi rūpajīvitindriyanti?",
  "kv8.10:7.8_0": "Āmantā.",
  "kv8.10:7.9_0": "Nirodhaṁ samāpannassa natthi jīvitindriyanti?",
  "kv8.10:7.10_0": "Na hevaṁ vattabbe …pe….",
  "kv8.10:8.1_0": "Nirodhaṁ samāpannassa atthi jīvitindriyanti?",
  "kv8.10:8.2_0": "Āmantā.",
  "kv8.10:8.3_0": "Hañci nirodhaṁ samāpannassa atthi jīvitindriyaṁ, no ca vata re vattabbe—",
  "kv8.10:8.4_0": "“natthi rūpajīvitindriyan”ti.",
  "kv8.10:9.1_0": "Nirodhaṁ samāpannassa atthi jīvitindriyanti?",
  "kv8.10:9.2_0": "Āmantā.",
  "kv8.10:9.3_0": "Katamakkhandhapariyāpannanti?",
  "kv8.10:9.4_0": "Saṅkhārakkhandhapariyāpannanti.",
  "kv8.10:9.5_0": "Nirodhaṁ samāpannassa atthi saṅkhārakkhandhoti?",
  "kv8.10:9.6_0": "Na hevaṁ vattabbe …pe….",
  "kv8.10:10.1_0": "Nirodhaṁ samāpannassa atthi saṅkhārakkhandhoti?",
  "kv8.10:10.2_0": "Āmantā.",
  "kv8.10:10.3_0": "Nirodhaṁ samāpannassa atthi vedanākkhandho …pe…",
  "kv8.10:10.4_0": "saññākkhandho …pe…",
  "kv8.10:10.5_0": "viññāṇakkhandhoti?",
  "kv8.10:10.6_0": "Na hevaṁ vattabbe …pe….",
  "kv8.10:11.1_0": "Nirodhaṁ samāpannassa atthi vedanākkhandho …pe…",
  "kv8.10:11.2_0": "saññākkhandho …pe…",
  "kv8.10:11.3_0": "viññāṇakkhandhoti?",
  "kv8.10:11.4_0": "Āmantā.",
  "kv8.10:11.5_0": "Na nirodhaṁ samāpannoti?",
  "kv8.10:11.6_0": "Na hevaṁ vattabbe …pe….",
  "kv8.10:12.1_0": "Natthi rūpajīvitindriyanti?",
  "kv8.10:12.2_0": "Āmantā.",
  "kv8.10:12.3_0": "Asaññasattānaṁ natthi jīvitindriyanti?",
  "kv8.10:12.4_0": "Na hevaṁ vattabbe …pe….",
  "kv8.10:13.1_0": "Asaññasattānaṁ atthi jīvitindriyanti?",
  "kv8.10:13.2_0": "Āmantā.",
  "kv8.10:13.3_0": "Hañci asaññasattānaṁ atthi jīvitindriyaṁ, no ca vata re vattabbe—",
  "kv8.10:13.4_0": "“natthi rūpajīvitindriyan”ti.",
  "kv8.10:13.5_0": "Asaññasattānaṁ atthi jīvitindriyanti?",
  "kv8.10:13.6_0": "Āmantā.",
  "kv8.10:13.7_0": "Katamakkhandhapariyāpannanti?",
  "kv8.10:13.8_0": "Saṅkhārakkhandhapariyāpannanti.",
  "kv8.10:13.9_0": "Asaññasattānaṁ atthi saṅkhārakkhandhoti?",
  "kv8.10:13.10_0": "Na hevaṁ vattabbe …pe….",
  "kv8.10:14.1_0": "Asaññasattānaṁ atthi saṅkhārakkhandhoti?",
  "kv8.10:14.2_0": "Āmantā.",
  "kv8.10:14.3_0": "Asaññasattānaṁ atthi vedanākkhandho …pe…",
  "kv8.10:14.4_0": "saññākkhandho …pe…",
  "kv8.10:14.5_0": "viññāṇakkhandhoti?",
  "kv8.10:14.6_0": "Na hevaṁ vattabbe …pe….",
  "kv8.10:15.1_0": "Asaññasattānaṁ atthi vedanākkhandho …pe…",
  "kv8.10:15.2_0": "saññākkhandho …pe…",
  "kv8.10:15.3_0": "viññāṇakkhandhoti?",
  "kv8.10:15.4_0": "Āmantā.",
  "kv8.10:15.5_0": "Pañcavokārabhavoti?",
  "kv8.10:15.6_0": "Na hevaṁ vattabbe …pe….",
  "kv8.10:16.1_0": "Upapattesiyena cittena samuṭṭhitaṁ jīvitindriyaṁ upapattesiye citte bhijjamāne ekadesaṁ bhijjatīti?",
  "kv8.10:16.2_0": "Āmantā.",
  "kv8.10:16.3_0": "Upapattesiyena cittena samuṭṭhito phasso upapattesiye citte bhijjamāne ekadeso bhijjatīti?",
  "kv8.10:16.4_0": "Na hevaṁ vattabbe …pe….",
  "kv8.10:17.1_0": "Upapattesiyena cittena samuṭṭhito phasso upapattesiye citte bhijjamāne anavaseso bhijjatīti?",
  "kv8.10:17.2_0": "Āmantā.",
  "kv8.10:17.3_0": "Upapattesiyena cittena samuṭṭhitaṁ jīvitindriyaṁ upapattesiye citte bhijjamāne anavasesaṁ bhijjatīti?",
  "kv8.10:17.4_0": "Na hevaṁ vattabbe …pe….",
  "kv8.10:18.1_0": "Dve jīvitindriyānīti?",
  "kv8.10:18.2_0": "Āmantā.",
  "kv8.10:18.3_0": "Dvīhi jīvitehi jīvati, dvīhi maraṇehi mīyatīti?",
  "kv8.10:18.4_0": "Āmantā.",
  "kv8.10:18.5_0": "Jīvitindriyakathā niṭṭhitā."
}