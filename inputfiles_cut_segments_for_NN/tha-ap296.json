{
  "tha-ap296:0.1_0": "Therāpadāna",
  "tha-ap296:0.2_0": "Citakapūjakavagga",
  "tha-ap296:0.3_0": "4. Saddasaññakattheraapadāna",
  "tha-ap296:1.1_0": "“Anuggatamhi ādicce,",
  "tha-ap296:1.2_0": "panādo vipulo ahu;",
  "tha-ap296:1.3_0": "Buddhaseṭṭhassa lokamhi,",
  "tha-ap296:1.4_0": "pātubhāvo mahesino.",
  "tha-ap296:2.1_0": "Ghosamassosahaṁ tattha,",
  "tha-ap296:2.2_0": "na ca passāmi taṁ jinaṁ;",
  "tha-ap296:2.3_0": "Maraṇañca anuppatto,",
  "tha-ap296:2.4_0": "buddhasaññamanussariṁ.",
  "tha-ap296:3.1_0": "Catunnavutito kappe,",
  "tha-ap296:3.2_0": "yaṁ saññamalabhiṁ tadā;",
  "tha-ap296:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap296:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap296:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap296:4.2_0": "…pe…",
  "tha-ap296:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap296:5.1_0": "Itthaṁ sudaṁ āyasmā saddasaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap296:6.1_0": "Saddasaññakattherassāpadānaṁ catutthaṁ."
}