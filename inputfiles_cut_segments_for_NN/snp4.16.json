{
  "snp4.16:0.1_0": "Sutta Nipāta 4.16",
  "snp4.16:0.2_0": "Sāriputtasutta",
  "snp4.16:1.1_0": "“Na me diṭṭho ito pubbe,",
  "snp4.16:1.2_0": "(iccāyasmā sāriputto)",
  "snp4.16:1.3_0": "Na suto uda kassaci;",
  "snp4.16:1.4_0": "Evaṁ vagguvado satthā,",
  "snp4.16:1.5_0": "Tusitā gaṇimāgato.",
  "snp4.16:2.1_0": "Sadevakassa lokassa,",
  "snp4.16:2.2_0": "yathā dissati cakkhumā;",
  "snp4.16:2.3_0": "Sabbaṁ tamaṁ vinodetvā,",
  "snp4.16:2.4_0": "ekova ratimajjhagā.",
  "snp4.16:3.1_0": "Taṁ buddhaṁ asitaṁ tādiṁ,",
  "snp4.16:3.2_0": "akuhaṁ gaṇimāgataṁ;",
  "snp4.16:3.3_0": "Bahūnamidha baddhānaṁ,",
  "snp4.16:3.4_0": "atthi pañhena āgamaṁ.",
  "snp4.16:4.1_0": "Bhikkhuno vijigucchato,",
  "snp4.16:4.2_0": "bhajato rittamāsanaṁ;",
  "snp4.16:4.3_0": "Rukkhamūlaṁ susānaṁ vā,",
  "snp4.16:4.4_0": "pabbatānaṁ guhāsu vā.",
  "snp4.16:5.1_0": "Uccāvacesu sayanesu,",
  "snp4.16:5.2_0": "kīvanto tattha bheravā;",
  "snp4.16:5.3_0": "Yehi bhikkhu na vedheyya,",
  "snp4.16:5.4_0": "nigghose sayanāsane.",
  "snp4.16:6.1_0": "Katī parissayā loke,",
  "snp4.16:6.2_0": "gacchato agataṁ disaṁ;",
  "snp4.16:6.3_0": "Ye bhikkhu abhisambhave,",
  "snp4.16:6.4_0": "pantamhi sayanāsane.",
  "snp4.16:7.1_0": "Kyāssa byappathayo assu,",
  "snp4.16:7.2_0": "kyāssassu idha gocarā;",
  "snp4.16:7.3_0": "Kāni sīlabbatānāssu,",
  "snp4.16:7.4_0": "pahitattassa bhikkhuno.",
  "snp4.16:8.1_0": "Kaṁ so sikkhaṁ samādāya,",
  "snp4.16:8.2_0": "ekodi nipako sato;",
  "snp4.16:8.3_0": "Kammāro rajatasseva,",
  "snp4.16:8.4_0": "niddhame malamattano”.",
  "snp4.16:9.1_0": "“Vijigucchamānassa yadidaṁ phāsu,",
  "snp4.16:9.2_0": "(sāriputtāti bhagavā)",
  "snp4.16:9.3_0": "Rittāsanaṁ sayanaṁ sevato ce;",
  "snp4.16:9.4_0": "Sambodhikāmassa yathānudhammaṁ,",
  "snp4.16:9.5_0": "Taṁ te pavakkhāmi yathā pajānaṁ.",
  "snp4.16:10.1_0": "Pañcannaṁ dhīro bhayānaṁ na bhāye,",
  "snp4.16:10.2_0": "Bhikkhu sato sapariyantacārī;",
  "snp4.16:10.3_0": "Ḍaṁsādhipātānaṁ sarīsapānaṁ,",
  "snp4.16:10.4_0": "Manussaphassānaṁ catuppadānaṁ.",
  "snp4.16:11.1_0": "Paradhammikānampi na santaseyya,",
  "snp4.16:11.2_0": "Disvāpi tesaṁ bahubheravāni;",
  "snp4.16:11.3_0": "Athāparāni abhisambhaveyya,",
  "snp4.16:11.4_0": "Parissayāni kusalānuesī.",
  "snp4.16:12.1_0": "Ātaṅkaphassena khudāya phuṭṭho,",
  "snp4.16:12.2_0": "Sītaṁ athuṇhaṁ adhivāsayeyya;",
  "snp4.16:12.3_0": "So tehi phuṭṭho bahudhā anoko,",
  "snp4.16:12.4_0": "Viriyaṁ parakkammadaḷhaṁ kareyya.",
  "snp4.16:13.1_0": "Theyyaṁ na kāre na musā bhaṇeyya,",
  "snp4.16:13.2_0": "Mettāya phasse tasathāvarāni;",
  "snp4.16:13.3_0": "Yadāvilattaṁ manaso vijaññā,",
  "snp4.16:13.4_0": "Kaṇhassa pakkhoti vinodayeyya.",
  "snp4.16:14.1_0": "Kodhātimānassa vasaṁ na gacche,",
  "snp4.16:14.2_0": "Mūlampi tesaṁ palikhañña tiṭṭhe;",
  "snp4.16:14.3_0": "Athappiyaṁ vā pana appiyaṁ vā,",
  "snp4.16:14.4_0": "Addhā bhavanto abhisambhaveyya.",
  "snp4.16:15.1_0": "Paññaṁ purakkhatvā kalyāṇapīti,",
  "snp4.16:15.2_0": "Vikkhambhaye tāni parissayāni;",
  "snp4.16:15.3_0": "Aratiṁ sahetha sayanamhi pante,",
  "snp4.16:15.4_0": "Caturo sahetha paridevadhamme.",
  "snp4.16:16.1_0": "Kiṁsū asissāmi kuva vā asissaṁ,",
  "snp4.16:16.2_0": "Dukkhaṁ vata settha kvajja sessaṁ;",
  "snp4.16:16.3_0": "Ete vitakke paridevaneyye,",
  "snp4.16:16.4_0": "Vinayetha sekho aniketacārī.",
  "snp4.16:17.1_0": "Annañca laddhā vasanañca kāle,",
  "snp4.16:17.2_0": "Mattaṁ so jaññā idha tosanatthaṁ;",
  "snp4.16:17.3_0": "So tesu gutto yatacāri gāme,",
  "snp4.16:17.4_0": "Rusitopi vācaṁ pharusaṁ na vajjā.",
  "snp4.16:18.1_0": "Okkhittacakkhu na ca pādalolo,",
  "snp4.16:18.2_0": "Jhānānuyutto bahujāgarassa;",
  "snp4.16:18.3_0": "Upekkhamārabbha samāhitatto,",
  "snp4.16:18.4_0": "Takkāsayaṁ kukkucciyūpachinde.",
  "snp4.16:19.1_0": "Cudito vacībhi satimābhinande,",
  "snp4.16:19.2_0": "Sabrahmacārīsu khilaṁ pabhinde;",
  "snp4.16:19.3_0": "Vācaṁ pamuñce kusalaṁ nātivelaṁ,",
  "snp4.16:19.4_0": "Janavādadhammāya na cetayeyya.",
  "snp4.16:20.1_0": "Athāparaṁ pañca rajāni loke,",
  "snp4.16:20.2_0": "Yesaṁ satīmā vinayāya sikkhe;",
  "snp4.16:20.3_0": "Rūpesu saddesu atho rasesu,",
  "snp4.16:20.4_0": "Gandhesu phassesu sahetha rāgaṁ.",
  "snp4.16:21.1_0": "Etesu dhammesu vineyya chandaṁ,",
  "snp4.16:21.2_0": "Bhikkhu satimā suvimuttacitto;",
  "snp4.16:21.3_0": "Kālena so sammā dhammaṁ parivīmaṁsamāno,",
  "snp4.16:21.4_0": "Ekodibhūto vihane tamaṁ so”ti.",
  "snp4.16:21.5_0": "Sāriputtasuttaṁ soḷasamaṁ.",
  "snp4.16:21.6_0": "Aṭṭhakavaggo catuttho.",
  "snp4.16:22.0_0": "Tassuddānaṁ",
  "snp4.16:22.1_0": "Kāmaṁ guhañca duṭṭhā ca,",
  "snp4.16:22.2_0": "suddhañca paramā jarā;",
  "snp4.16:22.3_0": "Metteyyo ca pasūro ca,",
  "snp4.16:22.4_0": "māgaṇḍi purābhedanaṁ.",
  "snp4.16:23.1_0": "Kalahaṁ dve ca byūhāni,",
  "snp4.16:23.2_0": "punadeva tuvaṭṭakaṁ;",
  "snp4.16:23.3_0": "Attadaṇḍavaraṁ suttaṁ,",
  "snp4.16:23.4_0": "therapuṭṭhena soḷasa;",
  "snp4.16:23.5_0": "Iti etāni suttāni,",
  "snp4.16:23.6_0": "sabbānaṭṭhakavaggikāti."
}