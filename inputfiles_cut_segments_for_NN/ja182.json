{
  "ja182:0.1_0": "Jātaka",
  "ja182:0.2_0": "Dukanipāta",
  "ja182:0.3_0": "Asadisavagga",
  "ja182:0.4_0": "2. Saṅgāmāvacarajātaka",
  "ja182:1.1_0": "“Saṅgāmāvacaro sūro,",
  "ja182:1.2_0": "balavā iti vissuto;",
  "ja182:1.3_0": "Kiṁ nu toraṇamāsajja,",
  "ja182:1.4_0": "paṭikkamasi kuñjara.",
  "ja182:2.1_0": "Omadda khippaṁ palighaṁ,",
  "ja182:2.2_0": "esikāni ca abbaha;",
  "ja182:2.3_0": "Toraṇāni ca madditvā,",
  "ja182:2.4_0": "khippaṁ pavisa kuñjarā”ti.",
  "ja182:3.1_0": "Saṅgāmāvacarajātakaṁ dutiyaṁ."
}