{
"kv22.1:1.1": "Kathāvatthu",
"kv22.1:2.1": "Khuddakaaḍḍhapaṇṇāsaka",
"kv22.1:3.1": "Bāvīsatimavagga",
"kv22.1:4.1": "Parinibbānakathā",
"kv22.1:5.1": "Atthi kiñci saṃyojanaṃ appahāya parinibbānanti?",
"kv22.1:5.2": "Āmantā.",
"kv22.1:5.3": "Atthi kiñci sakkāyadiṭṭhiṃ appahāya … pe … anottappaṃ appahāya parinibbānanti?",
"kv22.1:5.6": "Na hevaṃ vattabbe … pe ….",
"kv22.1:6.1": "Atthi kiñci saṃyojanaṃ appahāya parinibbānanti?",
"kv22.1:6.2": "Āmantā.",
"kv22.1:6.3": "Arahā sarāgo … pe … sakilesoti?",
"kv22.1:6.6": "Na hevaṃ vattabbe … pe …",
"kv22.1:6.8": "nanu arahā nirāgo … pe … nikkilesoti?",
"kv22.1:6.11": "Āmantā.",
"kv22.1:6.12": "Hañci arahā nirāgo … pe … nikkileso, no ca vata re vattabbe—",
"kv22.1:6.15": "“atthi kiñci saṃyojanaṃ appahāya parinibbānan”ti.",
"kv22.1:7.1": "☒ Na vattabbaṃ—",
"kv22.1:7.2": "“atthi kiñci saṃyojanaṃ appahāya parinibbānan”ti?",
"kv22.1:7.3": "Āmantā.",
"kv22.1:7.4": "Arahā sabbaṃ buddhavisayaṃ jānātīti?",
"kv22.1:7.5": "Na hevaṃ vattabbe.",
"kv22.1:7.6": "Tena hi atthi kiñci saṃyojanaṃ appahāya parinibbānanti.",
"kv22.1:8.1": "Parinibbānakathā niṭṭhitā."
}