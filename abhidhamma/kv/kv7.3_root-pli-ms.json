{
"kv7.3:1.1": "Kathāvatthu",
"kv7.3:2.1": "Mahāpaṇṇāsaka",
"kv7.3:3.1": "Sattamavagga",
"kv7.3:4.1": "Cetasikakathā",
"kv7.3:5.1": "Natthi cetasiko dhammoti?",
"kv7.3:5.2": "Āmantā.",
"kv7.3:5.3": "Nanu atthi keci dhammā cittena sahagatā sahajātā saṃsaṭṭhā sampayuttā ekuppādā ekanirodhā ekavatthukā ekārammaṇāti?",
"kv7.3:5.4": "Āmantā.",
"kv7.3:5.5": "Hañci atthi keci dhammā cittena sahagatā sahajātā saṃsaṭṭhā sampayuttā ekuppādā ekanirodhā ekavatthukā ekārammaṇā, no ca vata re vattabbe—",
"kv7.3:5.6": "“natthi cetasiko dhammo”ti.",
"kv7.3:6.1": "Phasso cittena sahajātoti?",
"kv7.3:6.2": "Āmantā.",
"kv7.3:6.3": "Hañci phasso cittena sahajāto, tena vata re vattabbe—",
"kv7.3:6.4": "“phasso cetasiko”ti.",
"kv7.3:6.5": "Vedanā … pe …",
"kv7.3:6.7": "saññā …",
"kv7.3:6.8": "cetanā …",
"kv7.3:6.9": "saddhā …",
"kv7.3:6.10": "vīriyaṃ …",
"kv7.3:6.11": "sati …",
"kv7.3:6.12": "samādhi …",
"kv7.3:6.13": "paññā …",
"kv7.3:6.14": "rāgo …",
"kv7.3:6.15": "doso …",
"kv7.3:6.16": "moho … pe …",
"kv7.3:6.18": "anottappaṃ cittena sahajātanti?",
"kv7.3:6.19": "Āmantā.",
"kv7.3:6.20": "Hañci anottappaṃ cittena sahajātaṃ, tena vata re vattabbe—",
"kv7.3:6.21": "“anottappaṃ cetasikan”ti.",
"kv7.3:7.1": "☒ Cittena sahajātāti katvā cetasikāti?",
"kv7.3:7.2": "Āmantā.",
"kv7.3:7.3": "Phassena sahajātāti katvā phassasikāti?",
"kv7.3:7.4": "Āmantā.",
"kv7.3:7.5": "Cittena sahajātāti katvā cetasikāti?",
"kv7.3:7.6": "Āmantā.",
"kv7.3:7.7": "Vedanāya …",
"kv7.3:7.8": "saññāya …",
"kv7.3:7.9": "cetanāya …",
"kv7.3:7.10": "saddhāya …",
"kv7.3:7.11": "vīriyena …",
"kv7.3:7.12": "satiyā …",
"kv7.3:7.13": "samādhinā …",
"kv7.3:7.14": "paññāya …",
"kv7.3:7.15": "rāgena …",
"kv7.3:7.16": "dosena …",
"kv7.3:7.17": "mohena … pe …",
"kv7.3:7.19": "anottappena sahajātāti katvā anottappāsikāti?",
"kv7.3:7.20": "Āmantā.",
"kv7.3:8.1": "Natthi cetasiko dhammoti?",
"kv7.3:8.2": "Āmantā.",
"kv7.3:8.3": "Nanu vuttaṃ bhagavatā—",
"kv7.3:9.1": "“Cittañhidaṃ cetasikā ca dhammā,",
"kv7.3:9.2": "Anattato saṃviditassa honti;",
"kv7.3:9.3": "Hīnappaṇītaṃ tadubhaye viditvā,",
"kv7.3:9.4": "Sammaddaso vedi palokadhamman”ti.",
"kv7.3:10.1": "Attheva suttantoti?",
"kv7.3:10.2": "Āmantā.",
"kv7.3:10.3": "Tena hi atthi cetasiko dhammoti.",
"kv7.3:11.1": "Natthi cetasiko dhammoti?",
"kv7.3:11.2": "Āmantā.",
"kv7.3:11.3": "Nanu vuttaṃ bhagavatā—",
"kv7.3:11.4": "“idha, kevaṭṭa, bhikkhu parasattānaṃ parapuggalānaṃ cittampi ādisati cetasikampi ādisati vitakkitampi ādisati vicāritampi ādisati—",
"kv7.3:11.5": "‘evampi te mano, itthampi te mano, itipi te cittan’”ti.",
"kv7.3:11.6": "Attheva suttantoti?",
"kv7.3:11.7": "Āmantā.",
"kv7.3:11.8": "Tena hi atthi cetasiko dhammoti.",
"kv7.3:12.1": "Cetasikakathā niṭṭhitā."
}