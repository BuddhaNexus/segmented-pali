{
"kv12.7:1.1": "Kathāvatthu",
"kv12.7:2.1": "Mahāpaṇṇāsaka",
"kv12.7:3.1": "Dvādasamavagga",
"kv12.7:4.1": "Ekabījīkathā",
"kv12.7:5.1": "☒ Na vattabbaṃ—",
"kv12.7:5.2": "“ekabījī puggalo ekabījitāniyato”ti?",
"kv12.7:5.3": "Āmantā.",
"kv12.7:5.4": "Nanu so ekabījīti?",
"kv12.7:5.5": "Āmantā.",
"kv12.7:5.6": "Hañci so ekabījī, tena vata re vattabbe—",
"kv12.7:5.7": "“ekabījī puggalo ekabījitāniyato”ti.",
"kv12.7:6.1": "Ekabījīkathā niṭṭhitā."
}