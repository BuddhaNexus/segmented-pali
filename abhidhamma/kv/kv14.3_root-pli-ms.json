{
"kv14.3:1.1": "Kathāvatthu",
"kv14.3:2.1": "Mahāpaṇṇāsaka",
"kv14.3:3.1": "Cuddasamavagga",
"kv14.3:4.1": "Anantarapaccayakathā",
"kv14.3:5.1": "Cakkhuviññāṇassa anantarā sotaviññāṇaṃ uppajjatīti?",
"kv14.3:5.2": "Āmantā.",
"kv14.3:5.3": "Yā cakkhuviññāṇassa uppādāya āvaṭṭanā … pe … paṇidhi, sāva sotaviññāṇassa uppādāya āvaṭṭanā … pe … paṇidhīti?",
"kv14.3:5.8": "Na hevaṃ vattabbe … pe ….",
"kv14.3:6.1": "Cakkhuviññāṇassa anantarā sotaviññāṇaṃ uppajjati, na vattabbaṃ—",
"kv14.3:6.2": "“yā cakkhuviññāṇassa uppādāya āvaṭṭanā … pe … paṇidhi, sāva sotaviññāṇassa uppādāya āvaṭṭanā … pe … paṇidhī”ti?",
"kv14.3:6.7": "Āmantā.",
"kv14.3:6.8": "Sotaviññāṇaṃ anāvaṭṭentassa uppajjati … pe …",
"kv14.3:6.10": "appaṇidahantassa uppajjatīti?",
"kv14.3:6.11": "Na hevaṃ vattabbe … pe ….",
"kv14.3:6.13": "Nanu sotaviññāṇaṃ āvaṭṭentassa uppajjati … pe …",
"kv14.3:6.15": "paṇidahantassa uppajjatīti?",
"kv14.3:6.16": "Āmantā.",
"kv14.3:6.17": "Hañci sotaviññāṇaṃ āvaṭṭentassa uppajjati … pe …",
"kv14.3:6.19": "paṇidahantassa uppajjati, no ca vata re vattabbe—",
"kv14.3:6.20": "“cakkhuviññāṇassa anantarā sotaviññāṇaṃ uppajjatī”ti.",
"kv14.3:7.1": "Cakkhuviññāṇassa anantarā sotaviññāṇaṃ uppajjatīti?",
"kv14.3:7.2": "Āmantā.",
"kv14.3:7.3": "Cakkhuviññāṇaṃ rūpanimittaṃ manasikaroto uppajjatīti?",
"kv14.3:7.4": "Āmantā.",
"kv14.3:7.5": "Sotaviññāṇaṃ rūpanimittaṃ manasikaroto uppajjatīti?",
"kv14.3:7.6": "Na hevaṃ vattabbe … pe ….",
"kv14.3:8.1": "Cakkhuviññāṇaṃ rūpārammaṇaññeva na aññārammaṇanti?",
"kv14.3:8.2": "Āmantā.",
"kv14.3:8.3": "Sotaviññāṇaṃ rūpārammaṇaññeva na aññārammaṇanti?",
"kv14.3:8.4": "Na hevaṃ vattabbe … pe ….",
"kv14.3:9.1": "Cakkhuñca paṭicca rūpe ca uppajjati cakkhuviññāṇanti?",
"kv14.3:9.2": "Āmantā.",
"kv14.3:9.3": "Cakkhuñca paṭicca rūpe ca uppajjati sotaviññāṇanti?",
"kv14.3:9.4": "Na hevaṃ vattabbe … pe ….",
"kv14.3:10.1": "Cakkhuñca paṭicca rūpe ca uppajjati sotaviññāṇanti?",
"kv14.3:10.2": "Āmantā.",
"kv14.3:10.3": "“Cakkhuñca paṭicca rūpe ca uppajjati sotaviññāṇan”ti—",
"kv14.3:10.4": "attheva suttantoti?",
"kv14.3:10.5": "Natthi.",
"kv14.3:10.6": "“Cakkhuñca paṭicca rūpe ca uppajjati cakkhuviññāṇan”ti—",
"kv14.3:10.7": "attheva suttantoti?",
"kv14.3:10.8": "Āmantā.",
"kv14.3:10.9": "Hañci “cakkhuñca paṭicca rūpe ca uppajjati cakkhuviññāṇan”ti—",
"kv14.3:10.10": "attheva suttanto, no ca vata re vattabbe—",
"kv14.3:10.11": "“cakkhuñca paṭicca rūpe ca uppajjati sotaviññāṇan”ti.",
"kv14.3:11.1": "Cakkhuviññāṇassa anantarā sotaviññāṇaṃ uppajjatīti?",
"kv14.3:11.2": "Āmantā.",
"kv14.3:11.3": "Taññeva cakkhuviññāṇaṃ taṃ sotaviññāṇanti?",
"kv14.3:11.4": "Na hevaṃ vattabbe … pe ….",
"kv14.3:12.1": "Sotaviññāṇassa anantarā ghānaviññāṇaṃ uppajjati … pe …",
"kv14.3:12.3": "ghānaviññāṇassa anantarā jivhāviññāṇaṃ uppajjati … pe …",
"kv14.3:12.5": "jivhāviññāṇassa anantarā kāyaviññāṇaṃ uppajjatīti?",
"kv14.3:12.6": "Āmantā.",
"kv14.3:12.7": "Yā jivhāviññāṇassa uppādāya āvaṭṭanā … pe … paṇidhi, sāva kāyaviññāṇassa uppādāya āvaṭṭanā … pe … paṇidhīti?",
"kv14.3:12.12": "Na hevaṃ vattabbe … pe …",
"kv14.3:12.14": "jivhāviññāṇassa anantarā kāyaviññāṇaṃ uppajjati, na vattabbaṃ—",
"kv14.3:12.15": "“yā jivhāviññāṇassa uppādāya āvaṭṭanā … pe … paṇidhi, sāva kāyaviññāṇassa uppādāya āvaṭṭanā … pe … paṇidhī”ti?",
"kv14.3:12.20": "Āmantā.",
"kv14.3:12.21": "Kāyaviññāṇaṃ anāvaṭṭentassa uppajjati … pe …",
"kv14.3:12.23": "appaṇidahantassa uppajjatīti?",
"kv14.3:12.24": "Na hevaṃ vattabbe … pe …",
"kv14.3:12.26": "nanu kāyaviññāṇaṃ āvaṭṭentassa uppajjati … pe …",
"kv14.3:12.28": "paṇidahantassa uppajjatīti?",
"kv14.3:12.29": "Āmantā.",
"kv14.3:12.30": "Hañci kāyaviññāṇaṃ āvaṭṭentassa uppajjati … pe …",
"kv14.3:12.32": "paṇidahantassa uppajjati, no ca vata re vattabbe—",
"kv14.3:12.33": "“jivhāviññāṇassa anantarā kāyaviññāṇaṃ uppajjatī”ti.",
"kv14.3:13.1": "Jivhāviññāṇassa anantarā kāyaviññāṇaṃ uppajjatīti?",
"kv14.3:13.2": "Āmantā.",
"kv14.3:13.3": "Jivhāviññāṇaṃ rasanimittaṃ manasikaroto uppajjatīti?",
"kv14.3:13.4": "Āmantā.",
"kv14.3:13.5": "Kāyaviññāṇaṃ rasanimittaṃ manasikaroto uppajjatīti?",
"kv14.3:13.6": "Na hevaṃ vattabbe … pe ….",
"kv14.3:14.1": "Jivhāviññāṇaṃ rasārammaṇaññeva na aññārammaṇanti?",
"kv14.3:14.2": "Āmantā.",
"kv14.3:14.3": "Kāyaviññāṇaṃ rasārammaṇaññeva na aññārammaṇanti?",
"kv14.3:14.4": "Na hevaṃ vattabbe … pe ….",
"kv14.3:15.1": "Jivhañca paṭicca rase ca uppajjati jivhāviññāṇanti?",
"kv14.3:15.2": "Āmantā.",
"kv14.3:15.3": "Jivhañca paṭicca rase ca uppajjati kāyaviññāṇanti?",
"kv14.3:15.4": "Na hevaṃ vattabbe … pe ….",
"kv14.3:16.1": "Jivhañca paṭicca rase ca uppajjati kāyaviññāṇanti?",
"kv14.3:16.2": "Āmantā.",
"kv14.3:16.3": "“Jivhañca paṭicca rase ca uppajjati kāyaviññāṇan”ti—",
"kv14.3:16.4": "attheva suttantoti?",
"kv14.3:16.5": "Natthi.",
"kv14.3:16.6": "“Jivhañca paṭicca rase ca uppajjati jivhāviññāṇan”ti—",
"kv14.3:16.7": "attheva suttantoti?",
"kv14.3:16.8": "Āmantā.",
"kv14.3:16.9": "Hañci “jivhañca paṭicca rase ca uppajjati jivhāviññāṇan”ti—",
"kv14.3:16.10": "attheva suttantoti, no ca vata re vattabbe—",
"kv14.3:16.11": "“jivhañca paṭicca rase ca uppajjati kāyaviññāṇan”ti.",
"kv14.3:17.1": "Jivhāviññāṇassa anantarā kāyaviññāṇaṃ uppajjatīti?",
"kv14.3:17.2": "Āmantā.",
"kv14.3:17.3": "Taññeva jivhāviññāṇaṃ taṃ kāyaviññāṇanti?",
"kv14.3:17.4": "Na hevaṃ vattabbe … pe ….",
"kv14.3:18.1": "☒ Na vattabbaṃ—",
"kv14.3:18.2": "“pañcaviññāṇā aññamaññassa samanantarā uppajjantī”ti?",
"kv14.3:18.3": "Āmantā.",
"kv14.3:18.4": "Nanu atthi koci naccati gāyati vādeti, rūpañca passati, saddañca suṇāti, gandhañca ghāyati, rasañca sāyati, phoṭṭhabbañca phusatīti?",
"kv14.3:18.5": "Āmantā.",
"kv14.3:18.6": "Hañci atthi koci naccati gāyati vādeti, rūpañca passati, saddañca suṇāti, gandhañca ghāyati, rasañca sāyati, phoṭṭhabbañca phusati, tena vata re vattabbe—",
"kv14.3:18.7": "“pañcaviññāṇā aññamaññassa samanantarā uppajjantī”ti.",
"kv14.3:19.1": "Anantarapaccayakathā niṭṭhitā."
}