{
"kv11.2:1.1": "Kathāvatthu",
"kv11.2:2.1": "Anusayapaṇṇāsaka",
"kv11.2:3.1": "Ekādasamavagga",
"kv11.2:4.1": "Ñāṇakathā",
"kv11.2:5.1": "Aññāṇe vigate ñāṇavippayutte citte vattamāne na vattabbaṃ—",
"kv11.2:5.2": "“ñāṇī”ti?",
"kv11.2:5.3": "Āmantā.",
"kv11.2:5.4": "Rāge vigate na vattabbaṃ—",
"kv11.2:5.5": "“vītarāgo”ti?",
"kv11.2:5.6": "Na hevaṃ vattabbe … pe …",
"kv11.2:5.8": "aññāṇe vigate ñāṇavippayutte citte vattamāne na vattabbaṃ—",
"kv11.2:5.9": "“ñāṇī”ti?",
"kv11.2:5.10": "Āmantā.",
"kv11.2:5.11": "Dose vigate …",
"kv11.2:5.12": "mohe vigate …",
"kv11.2:5.13": "kilese vigate na vattabbaṃ—",
"kv11.2:5.14": "“nikkileso”ti?",
"kv11.2:5.15": "Na hevaṃ vattabbe … pe ….",
"kv11.2:6.1": "Rāge vigate vattabbaṃ—",
"kv11.2:6.2": "“vītarāgo”ti?",
"kv11.2:6.3": "Āmantā.",
"kv11.2:6.4": "Aññāṇe vigate ñāṇavippayutte citte vattamāne vattabbaṃ—",
"kv11.2:6.5": "“ñāṇī”ti?",
"kv11.2:6.6": "Na hevaṃ vattabbe … pe …",
"kv11.2:6.8": "dose vigate …",
"kv11.2:6.9": "mohe vigate …",
"kv11.2:6.10": "kilese vigate vattabbaṃ—",
"kv11.2:6.11": "“nikkileso”ti?",
"kv11.2:6.12": "Āmantā.",
"kv11.2:6.13": "Aññāṇe vigate ñāṇavippayutte citte vattamāne vattabbaṃ—",
"kv11.2:6.14": "“ñāṇī”ti?",
"kv11.2:6.15": "Na hevaṃ vattabbe … pe ….",
"kv11.2:7.1": "☒ Aññāṇe vigate ñāṇavippayutte citte vattamāne vattabbaṃ—",
"kv11.2:7.2": "“ñāṇī”ti?",
"kv11.2:7.3": "Āmantā.",
"kv11.2:7.4": "Atītena ñāṇena ñāṇī niruddhena vigatena paṭipassaddhena ñāṇena ñāṇīti?",
"kv11.2:7.5": "Na hevaṃ vattabbe … pe ….",
"kv11.2:8.1": "Ñāṇakathā niṭṭhitā."
