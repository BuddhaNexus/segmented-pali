{
"kv15.2:1.1": "Kathāvatthu",
"kv15.2:2.1": "Mahāpaṇṇāsaka",
"kv15.2:3.1": "Pannarasamavagga",
"kv15.2:4.1": "Aññamaññapaccayakathā",
"kv15.2:5.1": "Avijjāpaccayāva saṅkhārā, na vattabbaṃ—",
"kv15.2:5.2": "“saṅkhārapaccayāpi avijjā”ti?",
"kv15.2:5.3": "Āmantā.",
"kv15.2:5.4": "Nanu avijjā saṅkhārena sahajātāti?",
"kv15.2:5.5": "Āmantā.",
"kv15.2:5.6": "Hañci avijjā saṅkhārena sahajātā, tena vata re vattabbe—",
"kv15.2:5.7": "“avijjāpaccayāpi saṅkhārā, saṅkhārapaccayāpi avijjā”ti.",
"kv15.2:6.1": "Taṇhāpaccayāva upādānaṃ, na vattabbaṃ—",
"kv15.2:6.2": "“upādānapaccayāpi taṇhā”ti?",
"kv15.2:6.3": "Āmantā.",
"kv15.2:6.4": "Nanu taṇhā upādānena sahajātāti?",
"kv15.2:6.5": "Āmantā.",
"kv15.2:6.6": "Hañci taṇhā upādānena sahajātā, tena vata re vattabbe—",
"kv15.2:6.7": "“taṇhāpaccayāpi upādānaṃ, upādānapaccayāpi taṇhā”ti.",
"kv15.2:7.1": "☒ “Jarāmaraṇapaccayā, bhikkhave, jāti, jātipaccayā bhavo”ti—",
"kv15.2:7.2": "attheva suttantoti?",
"kv15.2:7.3": "Natthi.",
"kv15.2:7.4": "Tena hi avijjāpaccayāva saṅkhārā, na vattabbaṃ—",
"kv15.2:7.5": "“saṅkhārapaccayāpi avijjā”ti.",
"kv15.2:7.6": "Taṇhāpaccayāva upādānaṃ, na vattabbaṃ—",
"kv15.2:7.7": "“upādānapaccayāpi taṇhā”ti.",
"kv15.2:8.1": "“Viññāṇapaccayā, bhikkhave, nāmarūpaṃ, nāmarūpapaccayāpi viññāṇan”ti—",
"kv15.2:8.2": "attheva suttantoti?",
"kv15.2:8.3": "Āmantā.",
"kv15.2:8.4": "Tena hi avijjāpaccayāpi saṅkhārā, saṅkhārapaccayāpi avijjā;",
"kv15.2:8.5": "taṇhāpaccayāpi upādānaṃ, upādānapaccayāpi taṇhāti.",
"kv15.2:9.1": "Aññamaññapaccayakathā niṭṭhitā."
}