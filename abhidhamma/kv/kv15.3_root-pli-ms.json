{
"kv15.3:1.1": "Kathāvatthu",
"kv15.3:2.1": "Mahāpaṇṇāsaka",
"kv15.3:3.1": "Pannarasamavagga",
"kv15.3:4.1": "Addhākathā",
"kv15.3:5.1": "Addhā parinipphannoti?",
"kv15.3:5.2": "Āmantā.",
"kv15.3:5.3": "Rūpanti?",
"kv15.3:5.4": "Na hevaṃ vattabbe … pe …",
"kv15.3:5.6": "vedanā … pe …",
"kv15.3:5.8": "saññā … pe …",
"kv15.3:5.10": "saṅkhārā … pe …",
"kv15.3:5.12": "viññāṇanti?",
"kv15.3:5.13": "Na hevaṃ vattabbe … pe …",
"kv15.3:5.15": "atīto addhā parinipphannoti?",
"kv15.3:5.16": "Āmantā.",
"kv15.3:5.17": "Rūpanti?",
"kv15.3:5.18": "Na hevaṃ vattabbe … pe …",
"kv15.3:5.20": "vedanā … pe …",
"kv15.3:5.22": "saññā … pe …",
"kv15.3:5.24": "saṅkhārā … pe …",
"kv15.3:5.26": "viññāṇanti?",
"kv15.3:5.27": "Na hevaṃ vattabbe … pe …",
"kv15.3:5.29": "anāgato addhā parinipphannoti?",
"kv15.3:5.30": "Āmantā.",
"kv15.3:5.31": "Rūpanti?",
"kv15.3:5.32": "Na hevaṃ vattabbe … pe …",
"kv15.3:5.34": "vedanā … pe …",
"kv15.3:5.36": "saññā … pe …",
"kv15.3:5.38": "saṅkhārā … pe …",
"kv15.3:5.40": "viññāṇanti?",
"kv15.3:5.41": "Na hevaṃ vattabbe … pe …",
"kv15.3:5.43": "paccuppanno addhā parinipphannoti?",
"kv15.3:5.44": "Āmantā.",
"kv15.3:5.45": "Rūpanti?",
"kv15.3:5.46": "Na hevaṃ vattabbe … pe …",
"kv15.3:5.48": "vedanā … pe …",
"kv15.3:5.50": "saññā … pe …",
"kv15.3:5.52": "saṅkhārā … pe …",
"kv15.3:5.54": "viññāṇanti?",
"kv15.3:5.55": "Na hevaṃ vattabbe … pe ….",
"kv15.3:6.1": "Atītaṃ rūpaṃ vedanā saññā saṅkhārā viññāṇaṃ atīto addhāti?",
"kv15.3:6.2": "Āmantā.",
"kv15.3:6.3": "Atītā pañcaddhāti?",
"kv15.3:6.4": "Na hevaṃ vattabbe … pe …",
"kv15.3:6.6": "anāgataṃ rūpaṃ vedanā saññā saṅkhārā viññāṇaṃ anāgato addhāti?",
"kv15.3:6.7": "Āmantā.",
"kv15.3:6.8": "Anāgatā pañcaddhāti?",
"kv15.3:6.9": "Na hevaṃ vattabbe … pe …",
"kv15.3:6.11": "paccuppannaṃ rūpaṃ vedanā saññā saṅkhārā viññāṇaṃ paccuppanno addhāti?",
"kv15.3:6.12": "Āmantā.",
"kv15.3:6.13": "Paccuppannā pañcaddhāti?",
"kv15.3:6.14": "Na hevaṃ vattabbe … pe ….",
"kv15.3:7.1": "Atītā pañcakkhandhā atīto addhā, anāgatā pañcakkhandhā anāgato addhā, paccuppannā pañcakkhandhā paccuppanno addhāti?",
"kv15.3:7.2": "Āmantā.",
"kv15.3:7.3": "Pannarasaddhāti?",
"kv15.3:7.4": "Na hevaṃ vattabbe … pe ….",
"kv15.3:8.1": "Atītāni dvādasāyatanāni atīto addhā, anāgatāni dvādasāyatanāni anāgato addhā, paccuppannāni dvādasāyatanāni paccuppanno addhāti?",
"kv15.3:8.2": "Āmantā.",
"kv15.3:8.3": "Chattiṃsa addhāti?",
"kv15.3:8.4": "Na hevaṃ vattabbe … pe ….",
"kv15.3:9.1": "Atītā aṭṭhārasa dhātuyo atīto addhā, anāgatā aṭṭhārasa dhātuyo anāgato addhā, paccuppannā aṭṭhārasa dhātuyo paccuppanno addhāti?",
"kv15.3:9.2": "Āmantā.",
"kv15.3:9.3": "Catupaññāsa addhāti?",
"kv15.3:9.4": "Na hevaṃ vattabbe … pe ….",
"kv15.3:10.1": "Atītāni bāvīsatindriyāni atīto addhā, anāgatāni bāvīsatindriyāni anāgato addhā, paccuppannāni bāvīsatindriyāni paccuppanno addhāti?",
"kv15.3:10.2": "Āmantā.",
"kv15.3:10.3": "Chasaṭṭhi addhāti?",
"kv15.3:10.4": "Na hevaṃ vattabbe … pe ….",
"kv15.3:11.1": "☒ Na vattabbaṃ—",
"kv15.3:11.2": "“addhā parinipphanno”ti?",
"kv15.3:11.3": "Āmantā.",
"kv15.3:11.4": "Nanu vuttaṃ bhagavatā—",
"kv15.3:11.5": "“tīṇimāni, bhikkhave, kathāvatthūni.",
"kv15.3:11.6": "Katamāni tīṇi?",
"kv15.3:11.7": "Atītaṃ vā, bhikkhave, addhānaṃ ārabbha kathaṃ katheyya—",
"kv15.3:11.8": "‘evaṃ ahosi atītamaddhānan’ti;",
"kv15.3:11.9": "anāgataṃ vā, bhikkhave, addhānaṃ ārabbha kathaṃ katheyya—",
"kv15.3:11.10": "‘evaṃ bhavissati anāgatamaddhānan’ti;",
"kv15.3:11.11": "etarahi vā, bhikkhave, paccuppannaṃ addhānaṃ ārabbha kathaṃ katheyya—",
"kv15.3:11.12": "‘evaṃ hoti etarahi paccuppannan’ti.",
"kv15.3:11.13": "Imāni kho, bhikkhave, tīṇi kathāvatthūnī”ti.",
"kv15.3:11.14": "Attheva suttantoti?",
"kv15.3:11.15": "Āmantā.",
"kv15.3:11.16": "Tena hi addhā parinipphannoti.",
"kv15.3:12.1": "Addhākathā niṭṭhitā."
}