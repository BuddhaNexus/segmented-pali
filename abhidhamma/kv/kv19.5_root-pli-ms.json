{
"kv19.5:1.1": "Kathāvatthu",
"kv19.5:2.1": "Mahāpaṇṇāsaka",
"kv19.5:3.1": "Ekūnavīsatimavagga",
"kv19.5:4.1": "Tathatākathā",
"kv19.5:5.1": "Sabbadhammānaṃ tathatā asaṅkhatāti?",
"kv19.5:5.2": "Āmantā.",
"kv19.5:5.3": "Nibbānaṃ … pe … amatanti?",
"kv19.5:5.6": "Na hevaṃ vattabbe … pe …",
"kv19.5:5.8": "sabbadhammānaṃ tathatā asaṅkhatā, nibbānaṃ asaṅkhatanti?",
"kv19.5:5.9": "Āmantā.",
"kv19.5:5.10": "Dve asaṅkhatānīti?",
"kv19.5:5.11": "Na hevaṃ vattabbe … pe …",
"kv19.5:5.13": "dve asaṅkhatānīti?",
"kv19.5:5.14": "Āmantā.",
"kv19.5:5.15": "Dve tāṇāni … pe … antarikā vāti?",
"kv19.5:5.18": "Na hevaṃ vattabbe … pe ….",
"kv19.5:6.1": "Rūpassa rūpatā, nanu rūpatā asaṅkhatāti?",
"kv19.5:6.2": "Āmantā.",
"kv19.5:6.3": "Nibbānaṃ … pe … amatanti?",
"kv19.5:6.6": "Na hevaṃ vattabbe … pe …",
"kv19.5:6.8": "rūpassa rūpatā, nanu rūpatā asaṅkhatā, nibbānaṃ asaṅkhatanti?",
"kv19.5:6.9": "Āmantā.",
"kv19.5:6.10": "Dve asaṅkhatānīti?",
"kv19.5:6.11": "Na hevaṃ vattabbe … pe …",
"kv19.5:6.13": "dve asaṅkhatānīti?",
"kv19.5:6.14": "Āmantā.",
"kv19.5:6.15": "Dve tāṇāni … pe … antarikā vāti?",
"kv19.5:6.18": "Na hevaṃ vattabbe … pe ….",
"kv19.5:7.1": "Vedanāya vedanatā, nanu vedanatā … pe …",
"kv19.5:7.3": "saññāya saññatā, nanu saññatā … pe …",
"kv19.5:7.5": "saṅkhārānaṃ saṅkhāratā, nanu saṅkhāratā … pe …",
"kv19.5:7.7": "viññāṇassa viññāṇatā, nanu viññāṇatā asaṅkhatāti?",
"kv19.5:7.8": "Āmantā.",
"kv19.5:7.9": "Nibbānaṃ … pe … amatanti?",
"kv19.5:7.12": "Na hevaṃ vattabbe … pe ….",
"kv19.5:8.1": "Rūpassa rūpatā, nanu rūpatā … pe …",
"kv19.5:8.3": "viññāṇassa viññāṇatā, nanu viññāṇatā asaṅkhatā, nibbānaṃ asaṅkhatanti?",
"kv19.5:8.4": "Āmantā.",
"kv19.5:8.5": "Cha asaṅkhatānīti?",
"kv19.5:8.6": "Na hevaṃ vattabbe … pe …",
"kv19.5:8.8": "cha asaṅkhatānīti?",
"kv19.5:8.9": "Āmantā.",
"kv19.5:8.10": "Cha tāṇāni … pe … antarikā vāti?",
"kv19.5:8.13": "Na hevaṃ vattabbe … pe ….",
"kv19.5:9.1": "☒ Na vattabbaṃ—",
"kv19.5:9.2": "“sabbadhammānaṃ tathatā asaṅkhatā”ti?",
"kv19.5:9.3": "Āmantā.",
"kv19.5:9.4": "Sabbadhammānaṃ tathatā rūpaṃ …",
"kv19.5:9.5": "vedanā …",
"kv19.5:9.6": "saññā …",
"kv19.5:9.7": "saṅkhārā …",
"kv19.5:9.8": "viññāṇanti?",
"kv19.5:9.9": "Na hevaṃ vattabbe.",
"kv19.5:9.10": "Tena hi sabbadhammānaṃ tathatā asaṅkhatāti.",
"kv19.5:10.1": "Tathatākathā niṭṭhitā."
}