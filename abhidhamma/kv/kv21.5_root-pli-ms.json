{
"kv21.5:1.1": "Kathāvatthu",
"kv21.5:2.1": "Mahāpaṇṇāsaka",
"kv21.5:3.1": "Ekavīsatimavagga",
"kv21.5:4.1": "Buddhakathā",
"kv21.5:5.1": "Atthi buddhānaṃ buddhehi hīnātirekatāti?",
"kv21.5:5.2": "Āmantā.",
"kv21.5:5.3": "Satipaṭṭhānatoti?",
"kv21.5:5.4": "Na hevaṃ vattabbe … pe …",
"kv21.5:5.6": "sammappadhānato … pe …",
"kv21.5:5.8": "iddhipādato …",
"kv21.5:5.9": "indriyato …",
"kv21.5:5.10": "balato …",
"kv21.5:5.11": "bojjhaṅgato …",
"kv21.5:5.12": "vasibhāvato … pe …",
"kv21.5:5.14": "sabbaññutañāṇadassanatoti?",
"kv21.5:5.15": "Na hevaṃ vattabbe … pe ….",
"kv21.5:6.1": "Buddhakathā niṭṭhitā."
}