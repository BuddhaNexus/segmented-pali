{
"kv6.1:1.1": "Kathāvatthu",
"kv6.1:2.1": "Niyāmapaṇṇāsaka",
"kv6.1:3.1": "Chaṭṭhavagga",
"kv6.1:4.1": "Niyāmakathā",
"kv6.1:5.1": "Niyāmo asaṅkhatoti?",
"kv6.1:5.2": "Āmantā.",
"kv6.1:5.3": "Nibbānaṃ tāṇaṃ leṇaṃ saraṇaṃ parāyanaṃ accutaṃ amatanti?",
"kv6.1:5.4": "Na hevaṃ vattabbe … pe ….",
"kv6.1:6.1": "Niyāmo asaṅkhato, nibbānaṃ asaṅkhatanti?",
"kv6.1:6.2": "Āmantā.",
"kv6.1:6.3": "Dve asaṅkhatānīti?",
"kv6.1:6.4": "Na hevaṃ vattabbe … pe ….",
"kv6.1:7.1": "Dve asaṅkhatānīti?",
"kv6.1:7.2": "Āmantā.",
"kv6.1:7.3": "Dve tāṇāni dve leṇāni dve saraṇāni dve parāyanāni dve accutāni dve amatāni dve nibbānānīti?",
"kv6.1:7.4": "Na hevaṃ vattabbe … pe ….",
"kv6.1:8.1": "Dve nibbānānīti?",
"kv6.1:8.2": "Āmantā.",
"kv6.1:8.3": "Atthi dvinnaṃ nibbānānaṃ uccanīcatā hīnapaṇītatā ukkaṃsāvakaṃso sīmā vā bhedo vā rāji vā antarikā vāti?",
"kv6.1:8.4": "Na hevaṃ vattabbe … pe ….",
"kv6.1:9.1": "Niyāmo asaṅkhatoti?",
"kv6.1:9.2": "Āmantā.",
"kv6.1:9.3": "Atthi keci niyāmaṃ okkamanti paṭilabhanti uppādenti samuppādenti uṭṭhāpenti samuṭṭhāpenti nibbattenti abhinibbattenti janenti sañjanentīti?",
"kv6.1:9.4": "Āmantā.",
"kv6.1:9.5": "Atthi keci asaṅkhataṃ okkamanti paṭilabhanti uppādenti samuppādenti uṭṭhāpenti samuṭṭhāpenti nibbattenti abhinibbattenti janenti sañjanentīti?",
"kv6.1:9.6": "Na hevaṃ vattabbe … pe ….",
"kv6.1:10.1": "Niyāmo asaṅkhatoti?",
"kv6.1:10.2": "Āmantā.",
"kv6.1:10.3": "Maggo asaṅkhatoti?",
"kv6.1:10.4": "Na hevaṃ vattabbe … pe ….",
"kv6.1:11.1": "Maggo saṅkhatoti?",
"kv6.1:11.2": "Āmantā.",
"kv6.1:11.3": "Niyāmo saṅkhatoti?",
"kv6.1:11.4": "Na hevaṃ vattabbe … pe ….",
"kv6.1:12.1": "Sotāpattiniyāmo asaṅkhatoti?",
"kv6.1:12.2": "Āmantā.",
"kv6.1:12.3": "Sotāpattimaggo asaṅkhatoti?",
"kv6.1:12.4": "Na hevaṃ vattabbe … pe ….",
"kv6.1:13.1": "Sotāpattimaggo saṅkhatoti?",
"kv6.1:13.2": "Āmantā.",
"kv6.1:13.3": "Sotāpattiniyāmo saṅkhatoti?",
"kv6.1:13.4": "Na hevaṃ vattabbe … pe ….",
"kv6.1:14.1": "Sakadāgāminiyāmo … pe …",
"kv6.1:14.3": "anāgāminiyāmo … pe …",
"kv6.1:14.5": "arahattaniyāmo asaṅkhatoti?",
"kv6.1:14.6": "Āmantā.",
"kv6.1:14.7": "Arahattamaggo asaṅkhatoti?",
"kv6.1:14.8": "Na hevaṃ vattabbe … pe …",
"kv6.1:14.10": "arahattamaggo saṅkhatoti?",
"kv6.1:14.11": "Āmantā.",
"kv6.1:14.12": "Arahattaniyāmo saṅkhatoti?",
"kv6.1:14.13": "Na hevaṃ vattabbe … pe ….",
"kv6.1:15.1": "Sotāpattiniyāmo asaṅkhato … pe …",
"kv6.1:15.3": "arahattaniyāmo asaṅkhato, nibbānaṃ asaṅkhatanti?",
"kv6.1:15.4": "Āmantā.",
"kv6.1:15.5": "Pañca asaṅkhatānīti?",
"kv6.1:15.6": "Na hevaṃ vattabbe … pe …",
"kv6.1:15.8": "pañca asaṅkhatānīti?",
"kv6.1:15.9": "Āmantā.",
"kv6.1:15.10": "Pañca tāṇāni … pe … antarikā vāti?",
"kv6.1:15.13": "Na hevaṃ vattabbe … pe ….",
"kv6.1:16.1": "Niyāmo asaṅkhatoti?",
"kv6.1:16.2": "Āmantā.",
"kv6.1:16.3": "Micchattaniyāmo asaṅkhatoti?",
"kv6.1:16.4": "Na hevaṃ vattabbe … pe …",
"kv6.1:16.6": "micchattaniyāmo saṅkhatoti?",
"kv6.1:16.7": "Āmantā.",
"kv6.1:16.8": "Sammattaniyāmo saṅkhatoti?",
"kv6.1:16.9": "Na hevaṃ vattabbe … pe ….",
"kv6.1:17.1": "☒ Na vattabbaṃ—",
"kv6.1:17.2": "“niyāmo asaṅkhato”ti?",
"kv6.1:17.3": "Āmantā.",
"kv6.1:17.4": "Niyāme uppajja niruddhe aniyato hotīti?",
"kv6.1:17.5": "Na hevaṃ vattabbe … pe ….",
"kv6.1:17.7": "Tena hi niyāmo asaṅkhatoti.",
"kv6.1:17.8": "Micchattaniyāme uppajja niruddhe aniyato hotīti?",
"kv6.1:17.9": "Na hevaṃ vattabbe … pe …",
"kv6.1:17.11": "tena hi micchattaniyāmo asaṅkhatoti.",
"kv6.1:18.1": "Niyāmakathā niṭṭhitā."
}