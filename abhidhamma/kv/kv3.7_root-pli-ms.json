{
"kv3.7:1.1": "Kathāvatthu",
"kv3.7:2.1": "Mahāpaṇṇāsaka",
"kv3.7:3.1": "Tatiyavagga",
"kv3.7:4.1": "Dibbacakkhukathā",
"kv3.7:5.1": "Maṃsacakkhuṃ dhammupatthaddhaṃ dibbacakkhuṃ hotīti?",
"kv3.7:5.2": "Āmantā.",
"kv3.7:5.3": "Maṃsacakkhuṃ dibbacakkhuṃ, dibbacakkhuṃ maṃsacakkhunti?",
"kv3.7:5.4": "Na hevaṃ vattabbe … pe ….",
"kv3.7:6.1": "Maṃsacakkhuṃ dhammupatthaddhaṃ dibbacakkhuṃ hotīti?",
"kv3.7:6.2": "Āmantā.",
"kv3.7:6.3": "Yādisaṃ maṃsacakkhuṃ tādisaṃ dibbacakkhuṃ, yādisaṃ dibbacakkhuṃ tādisaṃ maṃsacakkhunti?",
"kv3.7:6.4": "Na hevaṃ vattabbe … pe ….",
"kv3.7:7.1": "Maṃsacakkhuṃ dhammupatthaddhaṃ dibbacakkhuṃ hotīti?",
"kv3.7:7.2": "Āmantā.",
"kv3.7:7.3": "Taññeva maṃsacakkhuṃ taṃ dibbacakkhuṃ, taṃ dibbacakkhuṃ taṃ maṃsacakkhunti?",
"kv3.7:7.4": "Na hevaṃ vattabbe … pe ….",
"kv3.7:8.1": "Maṃsacakkhuṃ dhammupatthaddhaṃ dibbacakkhuṃ hotīti?",
"kv3.7:8.2": "Āmantā.",
"kv3.7:8.3": "Yādiso maṃsacakkhussa visayo ānubhāvo gocaro tādiso dibbassa cakkhussa visayo ānubhāvo gocaroti?",
"kv3.7:8.4": "Na hevaṃ vattabbe … pe ….",
"kv3.7:9.1": "Maṃsacakkhuṃ dhammupatthaddhaṃ dibbacakkhuṃ hotīti?",
"kv3.7:9.2": "Āmantā.",
"kv3.7:9.3": "Upādinnaṃ hutvā anupādinnaṃ hotīti?",
"kv3.7:9.4": "Na hevaṃ vattabbe … pe ….",
"kv3.7:10.1": "Upādinnaṃ hutvā anupādinnaṃ hotīti?",
"kv3.7:10.2": "Āmantā.",
"kv3.7:10.3": "Kāmāvacaraṃ hutvā rūpāvacaraṃ hotīti?",
"kv3.7:10.4": "Na hevaṃ vattabbe … pe ….",
"kv3.7:11.1": "Kāmāvacaraṃ hutvā rūpāvacaraṃ hotīti?",
"kv3.7:11.2": "Āmantā.",
"kv3.7:11.3": "Rūpāvacaraṃ hutvā arūpāvacaraṃ hotīti?",
"kv3.7:11.4": "Na hevaṃ vattabbe … pe ….",
"kv3.7:12.1": "Rūpāvacaraṃ hutvā arūpāvacaraṃ hotīti?",
"kv3.7:12.2": "Āmantā.",
"kv3.7:12.3": "Pariyāpannaṃ hutvā apariyāpannaṃ hotīti?",
"kv3.7:12.4": "Na hevaṃ vattabbe … pe ….",
"kv3.7:13.1": "Maṃsacakkhuṃ dhammupatthaddhaṃ dibbacakkhuṃ hotīti?",
"kv3.7:13.2": "Āmantā.",
"kv3.7:13.3": "Dibbacakkhuṃ dhammupatthaddhaṃ maṃsacakkhuṃ hotīti?",
"kv3.7:13.4": "Na hevaṃ vattabbe … pe ….",
"kv3.7:14.1": "Maṃsacakkhuṃ dhammupatthaddhaṃ dibbacakkhuṃ hotīti?",
"kv3.7:14.2": "Āmantā.",
"kv3.7:14.3": "Dibbacakkhuṃ dhammupatthaddhaṃ paññācakkhuṃ hotīti?",
"kv3.7:14.4": "Na hevaṃ vattabbe … pe ….",
"kv3.7:15.1": "Maṃsacakkhuṃ dhammupatthaddhaṃ dibbacakkhuṃ hotīti?",
"kv3.7:15.2": "Āmantā.",
"kv3.7:15.3": "Dibbacakkhuṃ dhammupatthaddhaṃ maṃsacakkhuṃ hotīti?",
"kv3.7:15.4": "Na hevaṃ vattabbe … pe ….",
"kv3.7:16.1": "Maṃsacakkhuṃ dhammupatthaddhaṃ dibbacakkhuṃ hotīti?",
"kv3.7:16.2": "Āmantā.",
"kv3.7:16.3": "Dveva cakkhūnīti?",
"kv3.7:16.4": "Na hevaṃ vattabbe … pe …",
"kv3.7:16.6": "dveva cakkhūnīti?",
"kv3.7:16.7": "Āmantā.",
"kv3.7:16.8": "Nanu tīṇi cakkhūni vuttāni bhagavatā—",
"kv3.7:16.9": "maṃsacakkhuṃ, dibbacakkhuṃ, paññācakkhunti?",
"kv3.7:16.10": "Āmantā.",
"kv3.7:16.11": "Hañci tīṇi cakkhūni vuttāni bhagavatā—",
"kv3.7:16.12": "maṃsacakkhuṃ, dibbacakkhuṃ, paññācakkhuṃ, no ca vata re vattabbe—",
"kv3.7:16.13": "“dveva cakkhūnī”ti.",
"kv3.7:17.1": "Dveva cakkhūnīti?",
"kv3.7:17.2": "Āmantā.",
"kv3.7:17.3": "Nanu vuttaṃ bhagavatā—",
"kv3.7:17.4": "“tīṇimāni, bhikkhave, cakkhūni.",
"kv3.7:17.5": "Katamāni tīṇi?",
"kv3.7:17.6": "Maṃsacakkhuṃ, dibbacakkhuṃ, paññācakkhunti—",
"kv3.7:17.7": "imāni kho, bhikkhave, tīṇi cakkhūnī”ti.",
"kv3.7:18.1": "“Maṃsacakkhuṃ dibbacakkhuṃ,",
"kv3.7:18.2": "Paññācakkhuṃ anuttaraṃ;",
"kv3.7:18.3": "Etāni tīṇi cakkhūni,",
"kv3.7:18.4": "Akkhāsi purisuttamo.",
"kv3.7:19.1": "Maṃsacakkhussa uppādo,",
"kv3.7:19.2": "maggo dibbassa cakkhuno;",
"kv3.7:19.3": "Yadā ca ñāṇaṃ udapādi,",
"kv3.7:19.4": "paññācakkhuṃ anuttaraṃ;",
"kv3.7:19.5": "Tassa cakkhussa paṭilābhā,",
"kv3.7:19.6": "sabbadukkhā pamuccatī”ti.",
"kv3.7:20.1": "Attheva suttantoti?",
"kv3.7:20.2": "Āmantā.",
"kv3.7:20.3": "Tena hi na vattabbaṃ—",
"kv3.7:20.4": "“dveva cakkhūnī”ti.",
"kv3.7:21.1": "Dibbacakkhukathā niṭṭhitā."
}