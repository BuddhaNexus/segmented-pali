{
"kv15.6:1.1": "Kathāvatthu",
"kv15.6:2.1": "Mahāpaṇṇāsaka",
"kv15.6:3.1": "Pannarasamavagga",
"kv15.6:4.1": "Jarāmaraṇakathā",
"kv15.6:5.1": "Lokuttarānaṃ dhammānaṃ jarāmaraṇaṃ lokuttaranti?",
"kv15.6:5.2": "Āmantā.",
"kv15.6:5.3": "Maggo phalaṃ nibbānaṃ, sotāpattimaggo sotāpattiphalaṃ … pe …",
"kv15.6:5.5": "bojjhaṅgoti?",
"kv15.6:5.6": "Na hevaṃ vattabbe … pe …",
"kv15.6:5.8": "sotāpattimaggassa jarāmaraṇaṃ sotāpattimaggoti?",
"kv15.6:5.9": "Na hevaṃ vattabbe … pe …",
"kv15.6:5.11": "sotāpattimaggassa jarāmaraṇaṃ sotāpattimaggoti?",
"kv15.6:5.12": "Āmantā.",
"kv15.6:5.13": "Sotāpattiphalassa jarāmaraṇaṃ sotāpattiphalanti?",
"kv15.6:5.14": "Na hevaṃ vattabbe … pe …",
"kv15.6:5.16": "sakadāgāmimaggassa … pe …",
"kv15.6:5.18": "sakadāgāmiphalassa … pe …",
"kv15.6:5.20": "anāgāmimaggassa … pe …",
"kv15.6:5.22": "anāgāmiphalassa … pe …",
"kv15.6:5.24": "arahattamaggassa jarāmaraṇaṃ arahattamaggoti?",
"kv15.6:5.25": "Na hevaṃ vattabbe … pe …",
"kv15.6:5.27": "arahattamaggassa jarāmaraṇaṃ arahattamaggoti?",
"kv15.6:5.28": "Āmantā.",
"kv15.6:5.29": "Arahattaphalassa jarāmaraṇaṃ arahattaphalanti?",
"kv15.6:5.30": "Na hevaṃ vattabbe … pe …",
"kv15.6:5.32": "satipaṭṭhānānaṃ …",
"kv15.6:5.33": "sammappadhānānaṃ …",
"kv15.6:5.34": "iddhipādānaṃ …",
"kv15.6:5.35": "indriyānaṃ …",
"kv15.6:5.36": "balānaṃ …",
"kv15.6:5.37": "bojjhaṅgānaṃ jarāmaraṇaṃ bojjhaṅgoti?",
"kv15.6:5.38": "Na hevaṃ vattabbe … pe ….",
"kv15.6:6.1": "☒ Na vattabbaṃ—",
"kv15.6:6.2": "“lokuttarānaṃ dhammānaṃ jarāmaraṇaṃ lokuttaran”ti?",
"kv15.6:6.3": "Āmantā.",
"kv15.6:6.4": "Lokiyanti?",
"kv15.6:6.5": "Na hevaṃ vattabbe.",
"kv15.6:6.6": "Tena hi lokuttaranti.",
"kv15.6:7.1": "Jarāmaraṇakathā niṭṭhitā."
}