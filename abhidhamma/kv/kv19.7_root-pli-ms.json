{
"kv19.7:1.1": "Kathāvatthu",
"kv19.7:2.1": "Mahāpaṇṇāsaka",
"kv19.7:3.1": "Ekūnavīsatimavagga",
"kv19.7:4.1": "Accantaniyāmakathā",
"kv19.7:5.1": "Atthi puthujjanassa accantaniyāmatāti?",
"kv19.7:5.2": "Āmantā.",
"kv19.7:5.4": "Mātughātako accantaniyato,",
"kv19.7:5.5": "pitughātako … pe …",
"kv19.7:5.6": "arahantaghātako … pe …",
"kv19.7:5.7": "ruhiruppādako … pe …",
"kv19.7:5.9": "saṃghabhedako accantaniyatoti?",
"kv19.7:5.10": "Na hevaṃ vattabbe … pe ….",
"kv19.7:6.1": "Atthi puthujjanassa accantaniyāmatāti?",
"kv19.7:6.2": "Āmantā.",
"kv19.7:6.3": "Accantaniyatassa puggalassa vicikicchā uppajjeyyāti?",
"kv19.7:6.4": "Āmantā.",
"kv19.7:6.5": "Hañci accantaniyatassa puggalassa vicikicchā uppajjeyya, no ca vata re vattabbe—",
"kv19.7:6.6": "“atthi puthujjanassa accantaniyāmatā”ti.",
"kv19.7:7.1": "Accantaniyatassa puggalassa vicikicchā nuppajjeyyāti?",
"kv19.7:7.2": "Āmantā.",
"kv19.7:7.3": "Pahīnāti?",
"kv19.7:7.4": "Na hevaṃ vattabbe … pe …",
"kv19.7:7.6": "pahīnāti?",
"kv19.7:7.7": "Āmantā.",
"kv19.7:7.8": "Sotāpattimaggenāti?",
"kv19.7:7.9": "Na hevaṃ vattabbe … pe …",
"kv19.7:7.11": "sakadāgāmimaggena … pe …",
"kv19.7:7.13": "anāgāmimaggena … pe …",
"kv19.7:7.15": "arahattamaggenāti?",
"kv19.7:7.16": "Na hevaṃ vattabbe … pe ….",
"kv19.7:8.1": "Katamena maggenāti?",
"kv19.7:8.2": "Akusalena maggenāti.",
"kv19.7:8.3": "Akusalo maggo niyyāniko khayagāmī bodhagāmī anāsavo … pe …",
"kv19.7:8.5": "asaṃkilesikoti?",
"kv19.7:8.6": "Na hevaṃ vattabbe … pe …",
"kv19.7:8.8": "nanu akusalo maggo aniyyāniko … pe …",
"kv19.7:8.10": "saṃkilesikoti?",
"kv19.7:8.11": "Āmantā.",
"kv19.7:8.12": "Hañci akusalo maggo aniyyāniko … pe …",
"kv19.7:8.14": "saṃkilesiko, no ca vata re vattabbe—",
"kv19.7:8.15": "“accantaniyatassa puggalassa vicikicchā akusalena maggena pahīnā”ti.",
"kv19.7:9.1": "Sassatadiṭṭhiyā niyatassa puggalassa ucchedadiṭṭhi uppajjeyyāti?",
"kv19.7:9.2": "Āmantā.",
"kv19.7:9.3": "Hañci sassatadiṭṭhiyā niyatassa puggalassa ucchedadiṭṭhi uppajjeyya, no ca vata re vattabbe—",
"kv19.7:9.4": "“atthi puthujjanassa accantaniyāmatā”ti.",
"kv19.7:10.1": "Sassatadiṭṭhiyā niyatassa puggalassa ucchedadiṭṭhi nuppajjeyyāti?",
"kv19.7:10.2": "Āmantā.",
"kv19.7:10.3": "Pahīnāti?",
"kv19.7:10.4": "Na hevaṃ vattabbe … pe …",
"kv19.7:10.6": "pahīnāti?",
"kv19.7:10.7": "Āmantā.",
"kv19.7:10.8": "Sotāpattimaggenāti?",
"kv19.7:10.9": "Na hevaṃ vattabbe … pe …",
"kv19.7:10.11": "sakadāgāmimaggena … pe …",
"kv19.7:10.13": "anāgāmimaggena … pe …",
"kv19.7:10.15": "arahattamaggenāti?",
"kv19.7:10.16": "Na hevaṃ vattabbe … pe ….",
"kv19.7:11.1": "Katamena maggenāti?",
"kv19.7:11.2": "Akusalena maggena.",
"kv19.7:11.3": "Akusalo maggo … pe …",
"kv19.7:11.5": "no ca vata re vattabbe—",
"kv19.7:11.6": "“sassatadiṭṭhiyā niyatassa puggalassa ucchedadiṭṭhi akusalena maggena pahīnā”ti.",
"kv19.7:12.1": "Ucchedadiṭṭhiyā niyatassa puggalassa sassatadiṭṭhi uppajjeyyāti?",
"kv19.7:12.2": "Āmantā.",
"kv19.7:12.3": "Hañci ucchedadiṭṭhiyā niyatassa puggalassa sassatadiṭṭhi uppajjeyya, no ca vata re vattabbe—",
"kv19.7:12.4": "“atthi puthujjanassa accantaniyāmatā”ti.",
"kv19.7:13.1": "Ucchedadiṭṭhiyā niyatassa puggalassa sassatadiṭṭhi nuppajjeyyāti?",
"kv19.7:13.2": "Āmantā.",
"kv19.7:13.3": "Pahīnāti?",
"kv19.7:13.4": "Na hevaṃ vattabbe … pe …",
"kv19.7:13.6": "pahīnāti?",
"kv19.7:13.7": "Āmantā.",
"kv19.7:13.8": "Sotāpattimaggenāti?",
"kv19.7:13.9": "Na hevaṃ vattabbe … pe …",
"kv19.7:13.11": "sakadāgāmimaggena … pe …",
"kv19.7:13.13": "anāgāmimaggena … pe …",
"kv19.7:13.15": "arahattamaggenāti?",
"kv19.7:13.16": "Na hevaṃ vattabbe … pe ….",
"kv19.7:14.1": "Katamena maggenāti?",
"kv19.7:14.2": "Akusalena maggenāti.",
"kv19.7:14.3": "Akusalo maggo … pe …",
"kv19.7:14.5": "no ca vata re vattabbe—",
"kv19.7:14.6": "“ucchedadiṭṭhiyā niyatassa puggalassa sassatadiṭṭhi akusalena maggena pahīnā”ti.",
"kv19.7:15.1": "☒ Na vattabbaṃ—",
"kv19.7:15.2": "“atthi puthujjanassa accantaniyāmatā”ti?",
"kv19.7:15.3": "Āmantā.",
"kv19.7:15.4": "Nanu vuttaṃ bhagavatā—",
"kv19.7:15.5": "“idha, bhikkhave, ekacco puggalo samannāgato hoti ekantakāḷakehi akusalehi dhammehi, so sakiṃ nimuggo nimuggova hotī”ti.",
"kv19.7:15.6": "Attheva suttantoti?",
"kv19.7:15.7": "Āmantā.",
"kv19.7:15.8": "Tena hi atthi puthujjanassa accantaniyāmatāti.",
"kv19.7:16.1": "Vuttaṃ bhagavatā—",
"kv19.7:16.2": "“idha, bhikkhave, ekacco puggalo samannāgato hoti ekantakāḷakehi akusalehi dhammehi, so sakiṃ nimuggo nimuggova hotī”ti katvā tena ca kāraṇena atthi puthujjanassa accantaniyāmatāti?",
"kv19.7:16.3": "Āmantā.",
"kv19.7:16.4": "Vuttaṃ bhagavatā—",
"kv19.7:16.5": "“idha, bhikkhave, ekacco puggalo ummujjitvā nimujjatī”ti.",
"kv19.7:16.6": "Attheva suttantoti?",
"kv19.7:16.7": "Āmantā.",
"kv19.7:16.8": "Sabbakālaṃ ummujjitvā nimujjatīti?",
"kv19.7:16.9": "Na hevaṃ vattabbe … pe ….",
"kv19.7:17.1": "Vuttaṃ bhagavatā—",
"kv19.7:17.2": "“idha, bhikkhave, ekacco puggalo samannāgato hoti ekantakāḷakehi akusalehi dhammehi, so sakiṃ nimuggo nimuggova hotī”ti—",
"kv19.7:17.3": "katvā tena ca kāraṇena atthi puthujjanassa accantaniyāmatāti?",
"kv19.7:17.4": "Āmantā.",
"kv19.7:17.5": "Vuttaṃ bhagavatā—",
"kv19.7:17.6": "“idha, bhikkhave, ekacco puggalo ummujjitvā ṭhito hoti, ummujjitvā vipassati viloketi, ummujjitvā patarati, ummujjitvā patigādhappatto hotī”ti.",
"kv19.7:17.7": "Attheva suttantoti?",
"kv19.7:17.8": "Āmantā.",
"kv19.7:17.9": "Sabbakālaṃ ummujjitvā patigādhappatto hotīti?",
"kv19.7:17.10": "Na hevaṃ vattabbe … pe ….",
"kv19.7:18.1": "Accantaniyāmakathā niṭṭhitā."
}