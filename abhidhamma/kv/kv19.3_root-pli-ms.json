{
"kv19.3:1.1": "Kathāvatthu",
"kv19.3:2.1": "Mahāpaṇṇāsaka",
"kv19.3:3.1": "Ekūnavīsatimavagga",
"kv19.3:4.1": "Sāmaññaphalakathā",
"kv19.3:5.1": "Sāmaññaphalaṃ asaṅkhatanti?",
"kv19.3:5.2": "Āmantā.",
"kv19.3:5.3": "Nibbānaṃ tāṇaṃ leṇaṃ saraṇaṃ parāyaṇaṃ accutaṃ amatanti?",
"kv19.3:5.4": "Na hevaṃ vattabbe … pe …",
"kv19.3:5.6": "sāmaññaphalaṃ asaṅkhataṃ, nibbānaṃ asaṅkhatanti?",
"kv19.3:5.7": "Āmantā.",
"kv19.3:5.8": "Dve asaṅkhatānīti?",
"kv19.3:5.9": "Na hevaṃ vattabbe … pe …",
"kv19.3:5.11": "dve asaṅkhatānīti?",
"kv19.3:5.12": "Āmantā.",
"kv19.3:5.13": "Dve tāṇāni … pe … antarikā vāti?",
"kv19.3:5.16": "Na hevaṃ vattabbe … pe ….",
"kv19.3:6.1": "Sāmaññaphalaṃ asaṅkhatanti?",
"kv19.3:6.2": "Āmantā.",
"kv19.3:6.3": "Sāmaññaṃ asaṅkhatanti?",
"kv19.3:6.4": "Na hevaṃ vattabbe … pe …",
"kv19.3:6.6": "sāmaññaṃ saṅkhatanti?",
"kv19.3:6.7": "Āmantā.",
"kv19.3:6.8": "Sāmaññaphalaṃ saṅkhatanti?",
"kv19.3:6.9": "Na hevaṃ vattabbe … pe ….",
"kv19.3:7.1": "Sotāpattiphalaṃ asaṅkhatanti?",
"kv19.3:7.2": "Āmantā.",
"kv19.3:7.3": "Sotāpattimaggo asaṅkhatoti?",
"kv19.3:7.4": "Na hevaṃ vattabbe … pe …",
"kv19.3:7.6": "sotāpattimaggo saṅkhatoti?",
"kv19.3:7.7": "Āmantā.",
"kv19.3:7.8": "Sotāpattiphalaṃ saṅkhatanti?",
"kv19.3:7.9": "Na hevaṃ vattabbe … pe ….",
"kv19.3:8.1": "Sakadāgāmiphalaṃ … pe …",
"kv19.3:8.3": "anāgāmiphalaṃ … pe …",
"kv19.3:8.5": "arahattaphalaṃ asaṅkhatanti?",
"kv19.3:8.6": "Āmantā.",
"kv19.3:8.7": "Arahattamaggo asaṅkhatoti?",
"kv19.3:8.8": "Na hevaṃ vattabbe … pe …",
"kv19.3:8.10": "arahattamaggo saṅkhatoti?",
"kv19.3:8.11": "Āmantā.",
"kv19.3:8.12": "Arahattaphalaṃ saṅkhatanti?",
"kv19.3:8.13": "Na hevaṃ vattabbe … pe ….",
"kv19.3:9.1": "Sotāpattiphalaṃ asaṅkhataṃ, sakadāgāmiphalaṃ … pe …",
"kv19.3:9.3": "anāgāmiphalaṃ … pe …",
"kv19.3:9.5": "arahattaphalaṃ asaṅkhataṃ, nibbānaṃ asaṅkhatanti?",
"kv19.3:9.6": "Āmantā.",
"kv19.3:9.7": "Pañca asaṅkhatānīti?",
"kv19.3:9.8": "Na hevaṃ vattabbe … pe …",
"kv19.3:9.10": "pañca asaṅkhatānīti?",
"kv19.3:9.11": "Āmantā.",
"kv19.3:9.12": "Pañca tāṇāni … pe … antarikā vāti?",
"kv19.3:9.15": "Na hevaṃ vattabbe … pe ….",
"kv19.3:10.1": "Sāmaññaphalakathā niṭṭhitā."
}