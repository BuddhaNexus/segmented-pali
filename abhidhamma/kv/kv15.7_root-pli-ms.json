{
"kv15.7:1.1": "Kathāvatthu",
"kv15.7:2.1": "Mahāpaṇṇāsaka",
"kv15.7:3.1": "Pannarasamavagga",
"kv15.7:4.1": "Saññāvedayitakathā",
"kv15.7:5.1": "Saññāvedayitanirodhasamāpatti lokuttarāti?",
"kv15.7:5.2": "Āmantā.",
"kv15.7:5.3": "Maggo phalaṃ nibbānaṃ, sotāpattimaggo sotāpattiphalaṃ … pe …",
"kv15.7:5.5": "bojjhaṅgoti?",
"kv15.7:5.6": "Na hevaṃ vattabbe … pe ….",
"kv15.7:6.1": "☒ Na vattabbaṃ—",
"kv15.7:6.2": "“saññāvedayitanirodhasamāpatti lokuttarā”ti?",
"kv15.7:6.3": "Āmantā.",
"kv15.7:6.4": "Lokiyāti?",
"kv15.7:6.5": "Na hevaṃ vattabbe.",
"kv15.7:6.6": "Tena hi lokuttarāti.",
"kv15.7:7.1": "Saññāvedayitakathā niṭṭhitā."
}