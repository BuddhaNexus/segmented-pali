{
"kv19.2:1.1": "Kathāvatthu",
"kv19.2:2.1": "Mahāpaṇṇāsaka",
"kv19.2:3.1": "Ekūnavīsatimavagga",
"kv19.2:4.1": "Suññatākathā",
"kv19.2:5.1": "Suññatā saṅkhārakkhandhapariyāpannāti?",
"kv19.2:5.2": "Āmantā.",
"kv19.2:5.3": "Animittaṃ saṅkhārakkhandhapariyāpannanti?",
"kv19.2:5.4": "Na hevaṃ vattabbe … pe …",
"kv19.2:5.6": "suññatā saṅkhārakkhandhapariyāpannāti?",
"kv19.2:5.7": "Āmantā.",
"kv19.2:5.8": "Appaṇihito saṅkhārakkhandhapariyāpannoti?",
"kv19.2:5.9": "Na hevaṃ vattabbe … pe …",
"kv19.2:5.11": "animittaṃ na vattabbaṃ—",
"kv19.2:5.12": "“saṅkhārakkhandhapariyāpannan”ti?",
"kv19.2:5.13": "Āmantā.",
"kv19.2:5.14": "Suññatā na vattabbā—",
"kv19.2:5.15": "“saṅkhārakkhandhapariyāpannā”ti?",
"kv19.2:5.16": "Na hevaṃ vattabbe … pe …",
"kv19.2:5.18": "appaṇihito na vattabbo—",
"kv19.2:5.19": "“saṅkhārakkhandhapariyāpanno”ti?",
"kv19.2:5.20": "Āmantā.",
"kv19.2:5.21": "Suññatā na vattabbā—",
"kv19.2:5.22": "“saṅkhārakkhandhapariyāpannā”ti?",
"kv19.2:5.23": "Na hevaṃ vattabbe … pe ….",
"kv19.2:6.1": "Suññatā saṅkhārakkhandhapariyāpannāti?",
"kv19.2:6.2": "Āmantā.",
"kv19.2:6.3": "Saṅkhārakkhandho na anicco na saṅkhato na paṭiccasamuppanno na khayadhammo na vayadhammo na virāgadhammo na nirodhadhammo na vipariṇāmadhammoti?",
"kv19.2:6.4": "Na hevaṃ vattabbe … pe …",
"kv19.2:6.6": "nanu saṅkhārakkhandho anicco saṅkhato paṭiccasamuppanno khayadhammo vayadhammo virāgadhammo nirodhadhammo vipariṇāmadhammoti?",
"kv19.2:6.7": "Āmantā.",
"kv19.2:6.8": "Hañci saṅkhārakkhandho anicco … pe …",
"kv19.2:6.10": "vipariṇāmadhammo, no ca vata re vattabbe—",
"kv19.2:6.11": "“suññatā saṅkhārakkhandhapariyāpannā”ti.",
"kv19.2:7.1": "Rūpakkhandhassa suññatā saṅkhārakkhandhapariyāpannāti?",
"kv19.2:7.2": "Āmantā.",
"kv19.2:7.3": "Saṅkhārakkhandhassa suññatā rūpakkhandhapariyāpannāti?",
"kv19.2:7.4": "Na hevaṃ vattabbe … pe …",
"kv19.2:7.6": "vedanākkhandhassa … pe …",
"kv19.2:7.8": "saññākkhandhassa …",
"kv19.2:7.9": "viññāṇakkhandhassa suññatā saṅkhārakkhandhapariyāpannāti?",
"kv19.2:7.10": "Āmantā.",
"kv19.2:7.11": "Saṅkhārakkhandhassa suññatā viññāṇakkhandhapariyāpannāti?",
"kv19.2:7.12": "Na hevaṃ vattabbe … pe ….",
"kv19.2:8.1": "Saṅkhārakkhandhassa suññatā na vattabbā—",
"kv19.2:8.2": "“rūpakkhandhapariyāpannā”ti?",
"kv19.2:8.3": "Āmantā.",
"kv19.2:8.4": "Rūpakkhandhassa suññatā na vattabbā—",
"kv19.2:8.5": "“saṅkhārakkhandhapariyāpannā”ti?",
"kv19.2:8.6": "Na hevaṃ vattabbe … pe …",
"kv19.2:8.8": "saṅkhārakkhandhassa suññatā na vattabbā—",
"kv19.2:8.9": "“vedanākkhandhapariyāpannā … pe …",
"kv19.2:8.11": "saññākkhandhapariyāpannā …",
"kv19.2:8.12": "viññāṇakkhandhapariyāpannā”ti?",
"kv19.2:8.13": "Āmantā.",
"kv19.2:8.14": "Viññāṇakkhandhassa suññatā na vattabbā—",
"kv19.2:8.15": "“saṅkhārakkhandhapariyāpannā”ti?",
"kv19.2:8.16": "Na hevaṃ vattabbe … pe ….",
"kv19.2:9.1": "☒ Na vattabbaṃ—",
"kv19.2:9.2": "“suññatā saṅkhārakkhandhapariyāpannā”ti?",
"kv19.2:9.3": "Āmantā.",
"kv19.2:9.4": "Nanu vuttaṃ bhagavatā—",
"kv19.2:9.5": "“suññamidaṃ, bhikkhave, saṅkhārā attena vā attaniyena vā”ti.",
"kv19.2:9.6": "Attheva suttantoti?",
"kv19.2:9.7": "Āmantā.",
"kv19.2:9.8": "Tena hi suññatā saṅkhārakkhandhapariyāpannāti.",
"kv19.2:10.1": "Suññatākathā niṭṭhitā."
}