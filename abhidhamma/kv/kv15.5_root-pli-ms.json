{
"kv15.5:1.1": "Kathāvatthu",
"kv15.5:2.1": "Mahāpaṇṇāsaka",
"kv15.5:3.1": "Pannarasamavagga",
"kv15.5:4.1": "Āsavakathā",
"kv15.5:5.1": "Cattāro āsavā anāsavāti?",
"kv15.5:5.2": "Āmantā.",
"kv15.5:5.3": "Maggo phalaṃ nibbānaṃ, sotāpattimaggo sotāpattiphalaṃ … pe …",
"kv15.5:5.5": "bojjhaṅgoti?",
"kv15.5:5.6": "Na hevaṃ vattabbe … pe ….",
"kv15.5:6.1": "☒ Na vattabbaṃ—",
"kv15.5:6.2": "“cattāro āsavā anāsavā”ti?",
"kv15.5:6.3": "Āmantā.",
"kv15.5:6.4": "Atthaññeva āsavā yehi āsavehi te āsavā sāsavā hontīti?",
"kv15.5:6.5": "Na hevaṃ vattabbe.",
"kv15.5:6.6": "Tena hi cattāro āsavā anāsavāti.",
"kv15.5:7.1": "Āsavakathā niṭṭhitā."
}