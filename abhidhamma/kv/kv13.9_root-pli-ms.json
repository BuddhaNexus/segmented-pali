{
"kv13.9:1.1": "Kathāvatthu",
"kv13.9:2.1": "Mahāpaṇṇāsaka",
"kv13.9:3.1": "Terasamavagga",
"kv13.9:4.1": "Dhammataṇhāabyākatātikathā",
"kv13.9:5.1": "Dhammataṇhā abyākatāti?",
"kv13.9:5.2": "Āmantā.",
"kv13.9:5.3": "Vipākābyākatā kiriyābyākatā rūpaṃ nibbānaṃ cakkhāyatanaṃ … pe … phoṭṭhabbāyatananti?",
"kv13.9:5.6": "Na hevaṃ vattabbe … pe ….",
"kv13.9:6.1": "Dhammataṇhā abyākatāti?",
"kv13.9:6.2": "Āmantā.",
"kv13.9:6.3": "Rūpataṇhā abyākatāti?",
"kv13.9:6.4": "Na hevaṃ vattabbe … pe … dhammataṇhā abyākatāti?",
"kv13.9:6.7": "Āmantā.",
"kv13.9:6.8": "Saddataṇhā … pe …",
"kv13.9:6.10": "gandhataṇhā … pe …",
"kv13.9:6.12": "rasataṇhā … pe …",
"kv13.9:6.14": "phoṭṭhabbataṇhā abyākatāti?",
"kv13.9:6.15": "Na hevaṃ vattabbe … pe ….",
"kv13.9:7.1": "Rūpataṇhā akusalāti?",
"kv13.9:7.2": "Āmantā.",
"kv13.9:7.3": "Dhammataṇhā akusalāti?",
"kv13.9:7.4": "Na hevaṃ vattabbe … pe …",
"kv13.9:7.6": "saddataṇhā … pe …",
"kv13.9:7.8": "phoṭṭhabbataṇhā akusalāti?",
"kv13.9:7.9": "Āmantā.",
"kv13.9:7.10": "Dhammataṇhā akusalāti?",
"kv13.9:7.11": "Na hevaṃ vattabbe … pe ….",
"kv13.9:8.1": "Dhammataṇhā abyākatāti?",
"kv13.9:8.2": "Āmantā.",
"kv13.9:8.3": "Nanu taṇhā akusalā vuttā bhagavatāti?",
"kv13.9:8.4": "Āmantā.",
"kv13.9:8.5": "Hañci taṇhā akusalā vuttā bhagavatā, no ca vata re vattabbe—",
"kv13.9:8.6": "“dhammataṇhā abyākatā”ti.",
"kv13.9:9.1": "Dhammataṇhā abyākatāti?",
"kv13.9:9.2": "Āmantā.",
"kv13.9:9.3": "Nanu lobho akusalo vutto bhagavatā, dhammataṇhā lobhoti?",
"kv13.9:9.4": "Āmantā.",
"kv13.9:9.5": "Hañci lobho akusalo vutto bhagavatā, dhammataṇhā lobho, no ca vata re vattabbe—",
"kv13.9:9.6": "“dhammataṇhā abyākatā”ti.",
"kv13.9:10.1": "Dhammataṇhā lobho abyākatoti?",
"kv13.9:10.2": "Āmantā.",
"kv13.9:10.3": "Rūpataṇhā lobho abyākatoti?",
"kv13.9:10.4": "Na hevaṃ vattabbe … pe … dhammataṇhā lobho abyākatoti?",
"kv13.9:10.7": "Āmantā.",
"kv13.9:10.8": "Saddataṇhā … pe …",
"kv13.9:10.10": "phoṭṭhabbataṇhā lobho abyākatoti?",
"kv13.9:10.11": "Na hevaṃ vattabbe … pe ….",
"kv13.9:11.1": "Rūpataṇhā lobho akusaloti?",
"kv13.9:11.2": "Āmantā.",
"kv13.9:11.3": "Dhammataṇhā lobho akusaloti?",
"kv13.9:11.4": "Na hevaṃ vattabbe … pe …",
"kv13.9:11.6": "saddataṇhā … pe …",
"kv13.9:11.8": "phoṭṭhabbataṇhā lobho akusaloti?",
"kv13.9:11.9": "Āmantā.",
"kv13.9:11.10": "Dhammataṇhā lobho akusaloti?",
"kv13.9:11.11": "Na hevaṃ vattabbe … pe ….",
"kv13.9:12.1": "Dhammataṇhā abyākatāti?",
"kv13.9:12.2": "Āmantā.",
"kv13.9:12.3": "Nanu vuttaṃ bhagavatā—",
"kv13.9:12.4": "“yāyaṃ taṇhā ponobbhavikā nandīrāgasahagatā tatratatrābhinandinī, seyyathidaṃ—",
"kv13.9:12.5": "kāmataṇhā, bhavataṇhā, vibhavataṇhā”ti.",
"kv13.9:12.6": "Attheva suttantoti?",
"kv13.9:12.7": "Āmantā.",
"kv13.9:12.8": "Tena hi na vattabbaṃ—",
"kv13.9:12.9": "“dhammataṇhā abyākatā”ti.",
"kv13.9:13.1": "☒ Na vattabbaṃ—",
"kv13.9:13.2": "“dhammataṇhā abyākatā”ti?",
"kv13.9:13.3": "Āmantā.",
"kv13.9:13.4": "Nanu sā dhammataṇhāti?",
"kv13.9:13.5": "Āmantā.",
"kv13.9:13.6": "Hañci sā dhammataṇhā, tena vata re vattabbe—",
"kv13.9:13.7": "“dhammataṇhā abyākatā”ti.",
"kv13.9:14.1": "Dhammataṇhā abyākatātikathā niṭṭhitā."
}