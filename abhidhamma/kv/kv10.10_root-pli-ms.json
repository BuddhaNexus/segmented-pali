{
"kv10.10:1.1": "Kathāvatthu",
"kv10.10:2.1": "Mahāpaṇṇāsaka",
"kv10.10:3.1": "Dasamavagga",
"kv10.10:4.1": "Viññattisīlantikathā",
"kv10.10:5.1": "Viññatti sīlanti?",
"kv10.10:5.2": "Āmantā.",
"kv10.10:5.3": "Pāṇātipātā veramaṇīti?",
"kv10.10:5.4": "Na hevaṃ vattabbe … pe …",
"kv10.10:5.6": "adinnādānā veramaṇīti?",
"kv10.10:5.7": "Na hevaṃ vattabbe … pe …",
"kv10.10:5.9": "kāmesumicchācārā veramaṇīti?",
"kv10.10:5.10": "Na hevaṃ vattabbe … pe …",
"kv10.10:5.12": "musāvādā veramaṇīti?",
"kv10.10:5.13": "Na hevaṃ vattabbe … pe …",
"kv10.10:5.15": "surāmerayamajjapamādaṭṭhānā veramaṇīti?",
"kv10.10:5.16": "Na hevaṃ vattabbe … pe ….",
"kv10.10:6.1": "Abhivādanaṃ sīlaṃ, paccuṭṭhānaṃ sīlaṃ, añjalikammaṃ sīlaṃ, sāmīcikammaṃ sīlaṃ, āsanābhihāro sīlaṃ, seyyābhihāro sīlaṃ, pādodakābhihāro sīlaṃ, pādakathalikābhihāro sīlaṃ, nhāne piṭṭhiparikammaṃ sīlanti?",
"kv10.10:6.2": "Āmantā.",
"kv10.10:6.3": "Pāṇātipātā veramaṇīti?",
"kv10.10:6.4": "Na hevaṃ vattabbe … pe …",
"kv10.10:6.6": "surāmerayamajjapamādaṭṭhānā veramaṇīti?",
"kv10.10:6.7": "Na hevaṃ vattabbe … pe ….",
"kv10.10:7.1": "☒ Na vattabbaṃ—",
"kv10.10:7.2": "“viññatti sīlan”ti?",
"kv10.10:7.3": "Āmantā.",
"kv10.10:7.4": "Dussilyanti?",
"kv10.10:7.5": "Na hevaṃ vattabbe … pe …",
"kv10.10:7.7": "tena hi viññatti sīlanti.",
"kv10.10:8.1": "Viññatti sīlantikathā niṭṭhitā."
}