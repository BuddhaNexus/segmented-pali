{
    "ai-ja134:0.1_0": "Jātaka",
    "ai-ja134:0.2_0": "Ekakanipāta",
    "ai-ja134:0.3_0": "Asampadānavagga",
    "ai-ja134:0.4_0": "4. Jhānasodhanajātaka",
    "ai-ja134:1.1_0": "“Those who see you as a ghost,",
    "ai-ja134:1.2_0": "and others, too, are hard to find.",
    "ai-ja134:1.3_0": "this world and the next,",
    "ai-ja134:1.4_0": "are fulfilled without blemish.”",
    "ai-ja134:2.1_0": "Absorption."
}