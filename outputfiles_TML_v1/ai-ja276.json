{
    "ai-ja276:0.1_0": "Jātaka",
    "ai-ja276:0.2_0": "Tikanipāta",
    "ai-ja276:0.3_0": "Udapānavagga",
    "ai-ja276:0.4_0": "6. Garudhammajātaka",
    "ai-ja276:1.1_0": "“Because of faith, ethics,",
    "ai-ja276:1.2_0": "for those who are virtuous,",
    "ai-ja276:1.3_0": "One whose appearance is unloved,",
    "ai-ja276:1.4_0": "like a cave-going an arrow.",
    "ai-ja276:2.1_0": "“They have no confidence,",
    "ai-ja276:2.2_0": "for whom,",
    "ai-ja276:2.3_0": "they’re acting like this—",
    "ai-ja276:2.4_0": "like birds in the past.",
    "ai-ja276:3.1_0": "O brahmin, I give you this dragon,",
    "ai-ja276:3.2_0": "a king of kings, so glorious.",
    "ai-ja276:3.3_0": "I grant you the opportunity",
    "ai-ja276:3.4_0": "But all of them came to enjoy sensual pleasures.”",
    "ai-ja276:4.1_0": "sixth hidden killer."
}