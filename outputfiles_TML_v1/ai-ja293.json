{
    "ai-ja293:0.1_0": "Jātaka",
    "ai-ja293:0.2_0": "Tikanipāta",
    "ai-ja293:0.3_0": "Kumbhavagga",
    "ai-ja293:0.4_0": "3. Kāyanibbindajātaka",
    "ai-ja293:1.1_0": "“I’ve seen this sickness",
    "ai-ja293:1.2_0": "is suffering by calming,",
    "ai-ja293:1.3_0": "Squirrels and bags,",
    "ai-ja293:1.4_0": "flowers from a fertile flower.",
    "ai-ja293:2.1_0": "lack of situational awareness,",
    "ai-ja293:2.2_0": "lovely,",
    "ai-ja293:2.3_0": "and oozing all over the place—",
    "ai-ja293:2.4_0": "for one who sees clearly.",
    "ai-ja293:3.1_0": "“For this rotten body!",
    "ai-ja293:3.2_0": "for those liable to disgust.",
    "ai-ja293:3.3_0": "People who determined",
    "ai-ja293:3.4_0": "the path to rebirth.",
    "ai-ja293:4.1_0": "and a third born of body made up."
}