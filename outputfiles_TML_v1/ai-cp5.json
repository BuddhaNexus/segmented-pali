{
    "ai-cp5:0.1_0": "Cariyāpiṭaka",
    "ai-cp5:0.2_0": "Akittivagga",
    "ai-cp5:0.3_0": "Dānapāramī 5",
    "ai-cp5:0.4_0": "Mahāgovindacariya",
    "ai-cp5:1.1_0": "“May the Brahmā realm be fully understood",
    "ai-cp5:1.2_0": "and razor for seven kings.",
    "ai-cp5:1.3_0": "don’t venerate the supreme person,",
    "ai-cp5:1.4_0": "the Great Steward!",
    "ai-cp5:2.1_0": "I fled seven chains",
    "ai-cp5:2.2_0": "that you feed me.",
    "ai-cp5:2.3_0": "I shall give you the great hermit,",
    "ai-cp5:2.4_0": "I’m unsurpassed,",
    "ai-cp5:3.1_0": "May he not give me some wealth, grain,",
    "ai-cp5:3.2_0": "nor do I know them.",
    "ai-cp5:3.3_0": "loved ones,",
    "ai-cp5:3.4_0": "That’s why I speak to you.”",
    "ai-cp5:4.1_0": "the Great Steward, the fifth."
}