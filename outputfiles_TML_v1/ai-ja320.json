{
    "ai-ja320:0.1_0": "Jātaka",
    "ai-ja320:0.2_0": "Catukkanipāta",
    "ai-ja320:0.3_0": "Pucimandavagga",
    "ai-ja320:0.4_0": "10. Succajajātaka",
    "ai-ja320:1.1_0": "“One who has conquered the earth,",
    "ai-ja320:1.2_0": "by speech, when two carcasses,",
    "ai-ja320:1.3_0": "But what do you think,",
    "ai-ja320:1.4_0": "in a mountain cave.”",
    "ai-ja320:2.1_0": "“But it is for you to say that",
    "ai-ja320:2.2_0": "Don’t say what you have.",
    "ai-ja320:2.3_0": "speaking Dhamma,",
    "ai-ja320:2.4_0": "one who talks without doing.”",
    "ai-ja320:3.1_0": "“You won’t O king,",
    "ai-ja320:3.2_0": "standing on the truth,",
    "ai-ja320:3.3_0": "Whoever has attained the goal,",
    "ai-ja320:3.4_0": "I delight in the truths!”",
    "ai-ja320:4.1_0": "“One poor, penniless,",
    "ai-ja320:4.2_0": "interest of rich people.",
    "ai-ja320:4.3_0": "For this is the ultimate wife,",
    "ai-ja320:4.4_0": "to live with women.”",
    "ai-ja320:5.1_0": "The tenth section on the pure.",
    "ai-ja320:5.2_0": "The second sub-chapter on fire is finished.",
    "ai-ja320:6.1_0": "Inspiring places",
    "ai-ja320:7.1_0": "As a bandit caught in his own form,",
    "ai-ja320:7.2_0": "fierce and harsh words,",
    "ai-ja320:7.3_0": "Happy and dead,",
    "ai-ja320:7.4_0": "like a lodh tree bark and those ten."
}