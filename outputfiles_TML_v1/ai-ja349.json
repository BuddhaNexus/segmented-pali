{
    "ai-ja349:0.1_0": "Jātaka",
    "ai-ja349:0.2_0": "Catukkanipāta",
    "ai-ja349:0.3_0": "Cūḷakuṇālavagga",
    "ai-ja349:0.4_0": "9. Sandhibhedajātaka",
    "ai-ja349:1.1_0": "“Mother, a woman,",
    "ai-ja349:1.2_0": "nor do I fear my charioteer.",
    "ai-ja349:1.3_0": "Overcome by immorality,",
    "ai-ja349:1.4_0": "See until they see clearly.",
    "ai-ja349:2.1_0": "My breasts have gone to me,",
    "ai-ja349:2.2_0": "overseerting.",
    "ai-ja349:2.3_0": "Bali’s army, and lion,",
    "ai-ja349:2.4_0": "fears have been born in Monatery.",
    "ai-ja349:3.1_0": "He sleeps on this bed,",
    "ai-ja349:3.2_0": "See this charioteer, charioteer!",
    "ai-ja349:3.3_0": "They speak with words,",
    "ai-ja349:3.4_0": "which purifys from divisive speech.",
    "ai-ja349:4.1_0": "Even those people make happiness",
    "ai-ja349:4.2_0": "returned to heaven,",
    "ai-ja349:4.3_0": "Those who speak on him,",
    "ai-ja349:4.4_0": "they don’t maid them.”",
    "ai-ja349:5.1_0": "transmigrating through countless rebirths."
}