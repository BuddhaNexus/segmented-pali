{
    "ai-thag6.3:0.1_0": "Verses of the Senior Monks",
    "ai-thag6.3:0.2_0": "The Book of the Sixes",
    "ai-thag6.3:0.3_0": "Chapter One",
    "ai-thag6.3:0.4_0": "6.3. Mahānāga",
    "ai-thag6.3:1.1_0": "Whoever has no respect",
    "ai-thag6.3:1.2_0": "you’re worthy of respects.",
    "ai-thag6.3:1.3_0": "falls away from the true teaching,",
    "ai-thag6.3:1.4_0": "like a fish in plenty of water.",
    "ai-thag6.3:2.1_0": "“Whoever has no respect",
    "ai-thag6.3:2.2_0": "you’re worthy of respects.",
    "ai-thag6.3:2.3_0": "They don’t thrive in the true teaching,",
    "ai-thag6.3:2.4_0": "like a seed in a field.",
    "ai-thag6.3:3.1_0": "“Whoever has no respect",
    "ai-thag6.3:3.2_0": "you’re worthy of respects.",
    "ai-thag6.3:3.3_0": "is far from being extinguished,",
    "ai-thag6.3:3.4_0": "in the teaching of the Dhamma king.",
    "ai-thag6.3:4.1_0": "“Whoever has no respect",
    "ai-thag6.3:4.2_0": "who serves them all?",
    "ai-thag6.3:4.3_0": "They don’t abandon the true teaching,",
    "ai-thag6.3:4.4_0": "like a fish in plenty of water.",
    "ai-thag6.3:5.1_0": "“Whoever has no respect",
    "ai-thag6.3:5.2_0": "who serves them all?",
    "ai-thag6.3:5.3_0": "thrives in the true teaching,",
    "ai-thag6.3:5.4_0": "like a fine seed in a field.",
    "ai-thag6.3:6.1_0": "“Whoever has no respect",
    "ai-thag6.3:6.2_0": "who serves them all?",
    "ai-thag6.3:6.3_0": "is close to quenching",
    "ai-thag6.3:6.4_0": "in the teaching of the Dhamma king.",
    "ai-thag6.3:6.5_0": "... a senior monk ..."
}