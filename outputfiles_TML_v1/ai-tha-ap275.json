{
    "ai-tha-ap275:0.1_0": "Therāpadāna",
    "ai-tha-ap275:0.2_0": "Suvaṇṇabibbohanavagga",
    "ai-tha-ap275:0.3_0": "3 Caṅkoṭakiyattheraapadāna",
    "ai-tha-ap275:1.1_0": "“Attaining the ocean,",
    "ai-tha-ap275:1.2_0": "in the mountain peak.",
    "ai-tha-ap275:1.3_0": "Having completed it,",
    "ai-tha-ap275:1.4_0": "a thorn canopy wrap.",
    "ai-tha-ap275:2.1_0": "He taught the great hermit,",
    "ai-tha-ap275:2.2_0": "compassionate for all beings:",
    "ai-tha-ap275:2.3_0": "It’s given a wrap garment,",
    "ai-tha-ap275:2.4_0": "He rejoices in heaven for an eon.",
    "ai-tha-ap275:3.1_0": "More than three eons",
    "ai-tha-ap275:3.2_0": "a thorn is humiliated.",
    "ai-tha-ap275:3.3_0": "I’m not aware of any bad places",
    "ai-tha-ap275:3.4_0": "And the fruit of wrap.",
    "ai-tha-ap275:4.1_0": "Attaining the four kinds of textual analysis",
    "ai-tha-ap275:4.2_0": "... “I attained the noble ones,” ...",
    "ai-tha-ap275:4.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap275:5.1_0": "That is how these verses were recited by the senior venerable Kaṅkiya.",
    "ai-tha-ap275:6.1_0": "a third, a thorn, a third, and a third part of senior monks."
}