{
    "ai-thag1.80:0.1_0": "Verses of the Senior Monks",
    "ai-thag1.80:0.2_0": "The Book of the Ones",
    "ai-thag1.80:0.3_0": "Chapter Eight",
    "ai-thag1.80:0.4_0": "1.80. Ugga",
    "ai-thag1.80:1.1_0": "Whatever actions I have performed,",
    "ai-thag1.80:1.2_0": "a little or a lot,",
    "ai-thag1.80:1.3_0": "are all completely exhausted:",
    "ai-thag1.80:1.4_0": "Now there is no further existence.”",
    "ai-thag1.80:1.5_0": "…",
    "ai-thag1.80:1.6_0": "as the senior monk Ugga of Hatthi.",
    "ai-thag1.80:1.7_0": "eighth of the fortnight.",
    "ai-thag1.80:2.0_0": "Inspiring places",
    "ai-thag1.80:2.1_0": "The senior monk who calf,",
    "ai-thag1.80:2.2_0": "Purāṇa gave him this.",
    "ai-thag1.80:2.3_0": "Sumanā, Susīma,",
    "ai-thag1.80:2.4_0": "one who loves the senior ones,",
    "ai-thag1.80:2.5_0": "Puṇḍoy is my son,",
    "ai-thag1.80:2.6_0": "guarded and protected."
}