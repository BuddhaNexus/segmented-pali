{
    "ai-tha-ap251:0.1_0": "Therāpadāna",
    "ai-tha-ap251:0.2_0": "Tuvaradāyakavagga",
    "ai-tha-ap251:0.3_0": "9 Bimbijāliyattheraapadāna",
    "ai-tha-ap251:1.1_0": "“Because they spring up,",
    "ai-tha-ap251:1.2_0": "Brahmadatta,",
    "ai-tha-ap251:1.3_0": "ruler of the four:",
    "ai-tha-ap251:1.4_0": "and what is the deathless state.",
    "ai-tha-ap251:2.1_0": "now the flowers of flowers",
    "ai-tha-ap251:2.2_0": "they crooked each other,",
    "ai-tha-ap251:2.3_0": "I bowed down to the Buddha,",
    "ai-tha-ap251:2.4_0": "the poised one.",
    "ai-tha-ap251:3.1_0": "Defiled by eight eons",
    "ai-tha-ap251:3.2_0": "presented with four friends,",
    "ai-tha-ap251:3.3_0": "Endowed with the seven gems,",
    "ai-tha-ap251:3.4_0": "the wheel-turning monarch’s bonfire,",
    "ai-tha-ap251:4.1_0": "Attaining the four kinds of textual analysis",
    "ai-tha-ap251:4.2_0": "... “I attained the noble ones,” ...",
    "ai-tha-ap251:4.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap251:5.1_0": "That is how these verses were recited by the senior venerable Bāhuka.",
    "ai-tha-ap251:6.1_0": "The ninth part of the senior monks."
}