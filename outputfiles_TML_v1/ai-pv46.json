{
    "ai-pv46:0.1_0": "Petavatthu",
    "ai-pv46:0.2_0": "Mahāvagga",
    "ai-pv46:0.3_0": "11. Pāṭaliputtapetavatthu",
    "ai-pv46:1.1_0": "“I’ve seen the animal realm,",
    "ai-pv46:1.2_0": "demons, dragons, and demons.",
    "ai-pv46:1.3_0": "“Living the fruit of deeds,",
    "ai-pv46:1.4_0": "I shall not mount him away.",
    "ai-pv46:1.5_0": "go there and get a skillful deed.”",
    "ai-pv46:2.1_0": "“You call me a native spirit,",
    "ai-pv46:2.2_0": "compassionate for all gods.",
    "ai-pv46:2.3_0": "I am saying,",
    "ai-pv46:2.4_0": "You are my teacher,",
    "ai-pv46:3.1_0": "I’ve seen the animal realms in hell,",
    "ai-pv46:3.2_0": "demons, dragons, and demons.",
    "ai-pv46:3.3_0": "“Living the fruit of deeds,",
    "ai-pv46:3.4_0": "I’m doing good deeds.”",
    "ai-pv46:4.1_0": "eleventh section."
}