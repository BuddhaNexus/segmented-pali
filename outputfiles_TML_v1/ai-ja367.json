{
    "ai-ja367:0.1_0": "Jātaka",
    "ai-ja367:0.2_0": "Pañcakanipāta",
    "ai-ja367:0.3_0": "Vaṇṇārohavagga",
    "ai-ja367:0.4_0": "7. Sāḷiyajātaka",
    "ai-ja367:1.1_0": "“The river of Āḷavī is beautiful.",
    "ai-ja367:1.2_0": "sent forth his dark army.",
    "ai-ja367:1.3_0": "with a demonstrable basis,",
    "ai-ja367:1.4_0": "who gave her this stain.",
    "ai-ja367:2.1_0": "burning up a great flood,",
    "ai-ja367:2.2_0": "who kills living creatures;",
    "ai-ja367:2.3_0": "So too, sleepily,",
    "ai-ja367:2.4_0": "And as a person kills themselves.",
    "ai-ja367:3.1_0": "killing and tormenting,",
    "ai-ja367:3.2_0": "who kills living creatures;",
    "ai-ja367:3.3_0": "So too, sleepily,",
    "ai-ja367:3.4_0": "And as a person kills themselves.",
    "ai-ja367:4.1_0": "A person then asked",
    "ai-ja367:4.2_0": "Regarded by the wind,",
    "ai-ja367:4.3_0": "hurt the dust to that same person,",
    "ai-ja367:4.4_0": "this person kills themselves.",
    "ai-ja367:5.1_0": "Whoever wrongs a man who has done no wrong,",
    "ai-ja367:5.2_0": "of the pure man and the blemish.",
    "ai-ja367:5.3_0": "hurt the fool",
    "ai-ja367:5.4_0": "like fine dust thrown upwind.”",
    "ai-ja367:6.1_0": "This was the seventh lost opportunity."
}