{
    "ai-ja234:0.1_0": "Jātaka",
    "ai-ja234:0.2_0": "Dukanipāta",
    "ai-ja234:0.3_0": "Upāhanavagga",
    "ai-ja234:0.4_0": "4. Asitābhūjātaka",
    "ai-ja234:1.1_0": "“Now, friend, you’ve been a fool,",
    "ai-ja234:1.2_0": "that you call sensual pleasures.",
    "ai-ja234:1.3_0": "the sorrowless, free of attachments,",
    "ai-ja234:1.4_0": "the lord of Vāsava.”",
    "ai-ja234:2.1_0": "“One who has desire to be reborn,",
    "ai-ja234:2.2_0": "and bears his final body.",
    "ai-ja234:2.3_0": "That’s how they create",
    "ai-ja234:2.4_0": "like I’m undisturbed.”",
    "ai-ja234:3.1_0": "with sorrow is at the fourth birth."
}