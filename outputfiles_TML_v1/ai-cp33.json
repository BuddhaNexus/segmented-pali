{
    "ai-cp33:0.1_0": "Cariyāpiṭaka",
    "ai-cp33:0.2_0": "Yudhañjayavagga",
    "ai-cp33:0.3_0": "Mettāpāramī 1",
    "ai-cp33:0.4_0": "Suvaṇṇasāmacariya",
    "ai-cp33:1.1_0": "I saw the noble ones",
    "ai-cp33:1.2_0": "the Buddha’s dispensation.",
    "ai-cp33:1.3_0": "in the wood, and the lion,",
    "ai-cp33:1.4_0": "I loved my thighs.",
    "ai-cp33:2.1_0": "Desirted by a lion,",
    "ai-cp33:2.2_0": "and headed by hermits.",
    "ai-cp33:2.3_0": "They’re like the sound of drums,",
    "ai-cp33:2.4_0": "in the forest grove.",
    "ai-cp33:3.1_0": "No-one was harming me",
    "ai-cp33:3.2_0": "but not anyone else.",
    "ai-cp33:3.3_0": "I love the power of love",
    "ai-cp33:3.4_0": "I enjoy myself in the forest.”",
    "ai-cp33:4.1_0": "gold, silver, and money."
}