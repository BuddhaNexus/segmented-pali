{
    "ai-tha-ap86:0.1_0": "Therāpadāna",
    "ai-tha-ap86:0.2_0": "Timiravagga",
    "ai-tha-ap86:0.3_0": "4. Adhopupphiyattheraapadāna",
    "ai-tha-ap86:1.1_0": "“The mendicant named Saddha,",
    "ai-tha-ap86:1.2_0": "Sikhī, a disciple of the best.",
    "ai-tha-ap86:1.3_0": "I am master of the three knowledges, mighty and powerful,",
    "ai-tha-ap86:1.4_0": "the Himalayas, the king of mountains.",
    "ai-tha-ap86:2.1_0": "I too am in the Himalayas,",
    "ai-tha-ap86:2.2_0": "so delightful.",
    "ai-tha-ap86:2.3_0": "unintelligent, unselfish,",
    "ai-tha-ap86:2.4_0": "Sujāpatī Gotamī.",
    "ai-tha-ap86:3.1_0": "Compassionate for all beings,",
    "ai-tha-ap86:3.2_0": "I achieved the mountain,",
    "ai-tha-ap86:3.3_0": "Taking a flower from flowers,",
    "ai-tha-ap86:3.4_0": "I go to the mountain.",
    "ai-tha-ap86:4.1_0": "Seven on flowers,",
    "ai-tha-ap86:4.2_0": "I lowered my final body.",
    "ai-tha-ap86:4.3_0": "giving,",
    "ai-tha-ap86:4.4_0": "and he left.",
    "ai-tha-ap86:5.1_0": "I’ve been that monastery;",
    "ai-tha-ap86:5.2_0": "and I became confident.",
    "ai-tha-ap86:5.3_0": "Taking axe and block,",
    "ai-tha-ap86:5.4_0": "I served the Mount Vepulla.",
    "ai-tha-ap86:6.1_0": "With the simile of the government official,",
    "ai-tha-ap86:6.2_0": "preparing a calf,",
    "ai-tha-ap86:6.3_0": "pus and blood,",
    "ai-tha-ap86:6.4_0": "I passed away there.",
    "ai-tha-ap86:7.1_0": "The lie down on the same sheet",
    "ai-tha-ap86:7.2_0": "flowering on a flower",
    "ai-tha-ap86:7.3_0": "I’m not aware of any bad places",
    "ai-tha-ap86:7.4_0": "flowers.",
    "ai-tha-ap86:8.1_0": "Attaining the four kinds of textual analysis",
    "ai-tha-ap86:8.2_0": "... “I attained the noble ones,” ...",
    "ai-tha-ap86:8.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap86:9.1_0": "That is how these verses were recited by the senior venerable Nārada.",
    "ai-tha-ap86:10.1_0": "The senior monk with flowers."
}