{
    "ai-ja28:0.1_0": "Jātaka",
    "ai-ja28:0.2_0": "Ekakanipāta",
    "ai-ja28:0.3_0": "Kuruṅgavagga",
    "ai-ja28:0.4_0": "8. Nandivisālajātaka",
    "ai-ja28:1.1_0": "“Mother, you should speak to me",
    "ai-ja28:1.2_0": "Never what’s unpleasant.",
    "ai-ja28:1.3_0": "Remember the mind,",
    "ai-ja28:1.4_0": "Heavy loads got carried,",
    "ai-ja28:1.5_0": "And he obtained wealth;",
    "ai-ja28:1.6_0": "And he was delighted with that.’",
    "ai-ja28:2.1_0": "The eighth sub-chapter on relishing is finished."
}