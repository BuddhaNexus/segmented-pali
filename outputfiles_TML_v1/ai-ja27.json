{
    "ai-ja27:0.1_0": "Jātaka",
    "ai-ja27:0.2_0": "Ekakanipāta",
    "ai-ja27:0.3_0": "Kuruṅgavagga",
    "ai-ja27:0.4_0": "7. Abhiṇhajātaka",
    "ai-ja27:1.1_0": "“What’s the point of a mouthful that’s too large,",
    "ai-ja27:1.2_0": "but not your alms-food.",
    "ai-ja27:1.3_0": "I am quite confident,",
    "ai-ja27:1.4_0": "like a bull elephant who struck?”",
    "ai-ja27:2.1_0": "frequently visiting each other."
}