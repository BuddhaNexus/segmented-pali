{
    "ai-ja213:0.1_0": "Jātaka",
    "ai-ja213:0.2_0": "Dukanipāta",
    "ai-ja213:0.3_0": "Bīraṇathambhavagga",
    "ai-ja213:0.4_0": "3. Bharujātaka",
    "ai-ja213:1.1_0": "“The gap between the rocks",
    "ai-ja213:1.2_0": "merchants,",
    "ai-ja213:1.3_0": "The rain comes with dried,",
    "ai-ja213:1.4_0": "that has gone beyond rebirth.",
    "ai-ja213:2.1_0": "Therefore, consent",
    "ai-ja213:2.2_0": "the astute do not regard them well,",
    "ai-ja213:2.3_0": "Speech the mind with no anger,",
    "ai-ja213:2.4_0": "is truly understood the truth of nature.”",
    "ai-ja213:3.1_0": "third from fear."
}