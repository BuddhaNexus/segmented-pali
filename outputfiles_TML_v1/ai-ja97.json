{
    "ai-ja97:0.1_0": "Jātaka",
    "ai-ja97:0.2_0": "Ekakanipāta",
    "ai-ja97:0.3_0": "Littavagga",
    "ai-ja97:0.4_0": "7. Nāmasiddhijātaka",
    "ai-ja97:1.1_0": "Having seen the dead,",
    "ai-ja97:1.2_0": "so hard to come, hard to pass.",
    "ai-ja97:1.3_0": "whereas, and a deluded person,",
    "ai-ja97:1.4_0": "they’re truly a bad one.”",
    "ai-ja97:2.1_0": "grain was born in the seventh direction."
}