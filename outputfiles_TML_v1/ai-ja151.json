{
    "ai-ja151:0.1_0": "Jātaka",
    "ai-ja151:0.2_0": "Dukanipāta",
    "ai-ja151:0.3_0": "Daḷhavagga",
    "ai-ja151:0.4_0": "1. Rājovādajātaka",
    "ai-ja151:1.1_0": "“I shall strongly fasten to a strong bow,",
    "ai-ja151:1.2_0": "my tender, tender,",
    "ai-ja151:1.3_0": "doing good,",
    "ai-ja151:1.4_0": "viceroyed,",
    "ai-ja151:1.5_0": "Like this king,",
    "ai-ja151:1.6_0": "the mind can find a vulnerability.”",
    "ai-ja151:2.1_0": "Outside anger has arisen,",
    "ai-ja151:2.2_0": "villainy with virtue,",
    "ai-ja151:2.3_0": "stinginess with giving,",
    "ai-ja151:2.4_0": "and lies with truth.",
    "ai-ja151:2.5_0": "Like this king,",
    "ai-ja151:2.6_0": "and delights in the path.”",
    "ai-ja151:3.1_0": "First the King of Magadha was an adulter."
}