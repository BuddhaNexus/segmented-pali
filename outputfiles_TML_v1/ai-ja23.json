{
    "ai-ja23:0.1_0": "Jātaka",
    "ai-ja23:0.2_0": "Ekakanipāta",
    "ai-ja23:0.3_0": "Kuruṅgavagga",
    "ai-ja23:0.4_0": "3. Bhojājānīyajātaka",
    "ai-ja23:1.1_0": "“Hastening by carriage,",
    "ai-ja23:1.2_0": "that dart,",
    "ai-ja23:1.3_0": "It’s better to feed the sacred flame,",
    "ai-ja23:1.4_0": "like I enjoy my sweets.”",
    "ai-ja23:2.1_0": "a thoroughbred checked by a thoroughbred."
}