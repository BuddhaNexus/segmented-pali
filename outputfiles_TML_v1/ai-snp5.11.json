{
    "ai-snp5.11:0.1_0": "Sutta Nipāta 5.11",
    "ai-snp5.11:0.2_0": "Kappamāṇavapucchā",
    "ai-snp5.11:1.1_0": "“Living in the open,",
    "ai-snp5.11:1.2_0": "said Venerable Kappa,",
    "ai-snp5.11:1.3_0": "the great floods of the great hermits,",
    "ai-snp5.11:1.4_0": "Worthy to old age,",
    "ai-snp5.11:1.5_0": "quickly clear and vegetable,",
    "ai-snp5.11:1.6_0": "Teach me the Dhamma,",
    "ai-snp5.11:1.7_0": "as they had been realized.”",
    "ai-snp5.11:2.1_0": "“Living in the open,",
    "ai-snp5.11:2.2_0": "said the Buddha to Sela,",
    "ai-snp5.11:2.3_0": "the great floods of the great hermits,",
    "ai-snp5.11:2.4_0": "Worthy to old age,",
    "ai-snp5.11:2.5_0": "your savings will move.",
    "ai-snp5.11:3.1_0": "one with nothing, by not grasping:",
    "ai-snp5.11:3.2_0": "they’ve realized this way:",
    "ai-snp5.11:3.3_0": "I declare it is extinguishment",
    "ai-snp5.11:3.4_0": "holding birth and death at bait.",
    "ai-snp5.11:4.1_0": "Those who have fully understood the mindful,",
    "ai-snp5.11:4.2_0": "in those who are extinguished,",
    "ai-snp5.11:4.3_0": "don’t fall under Māra’s sway,",
    "ai-snp5.11:4.4_0": "they don’t become your footmen.”",
    "ai-snp5.11:4.5_0": "And the ten years of seniority."
}