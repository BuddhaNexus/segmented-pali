{
    "ai-ja98:0.1_0": "Jātaka",
    "ai-ja98:0.2_0": "Ekakanipāta",
    "ai-ja98:0.3_0": "Littavagga",
    "ai-ja98:0.4_0": "8. Kūṭavāṇijajātaka",
    "ai-ja98:1.1_0": "“It’s good that my child’s so astute,",
    "ai-ja98:1.2_0": "not yet the astute person is astute.",
    "ai-ja98:1.3_0": "With their son Tissa,",
    "ai-ja98:1.4_0": "knowing the mind gone there.",
    "ai-ja98:2.1_0": "Eightteens of buffaloes."
}