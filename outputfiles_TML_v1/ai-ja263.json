{
    "ai-ja263:0.1_0": "Jātaka",
    "ai-ja263:0.2_0": "Tikanipāta",
    "ai-ja263:0.3_0": "Padumavagga",
    "ai-ja263:0.4_0": "3. Cūḷapalobhanajātaka",
    "ai-ja263:1.1_0": "That’s what happens for you,",
    "ai-ja263:1.2_0": "relying on a psychic power.",
    "ai-ja263:1.3_0": "Having gone to a charnel ground",
    "ai-ja263:1.4_0": "they sink down in the forest.",
    "ai-ja263:2.1_0": "It’s been converted by the great flood,",
    "ai-ja263:2.2_0": "the spiritual life,",
    "ai-ja263:2.3_0": "When they know the lion’s roar,",
    "ai-ja263:2.4_0": "you should avoid from afar.",
    "ai-ja263:3.1_0": "Whatever it trembles with",
    "ai-ja263:3.2_0": "or by means of desire,",
    "ai-ja263:3.3_0": "are the only one born,",
    "ai-ja263:3.4_0": "quickly break apart.”",
    "ai-ja263:4.1_0": "The Shorter Discourse with Mālaka."
}