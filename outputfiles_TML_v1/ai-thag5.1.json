{
    "ai-thag5.1:0.1_0": "Verses of the Senior Monks",
    "ai-thag5.1:0.2_0": "The Book of the Fives",
    "ai-thag5.1:0.3_0": "Chapter One",
    "ai-thag5.1:0.4_0": "5.1. Rājadatta",
    "ai-thag5.1:1.1_0": "I, a monk, went to a charnel ground",
    "ai-thag5.1:1.2_0": "saw a woman wearing away.",
    "ai-thag5.1:1.3_0": "that’s stuck in my body,",
    "ai-thag5.1:1.4_0": "The beatens and drumpeds.",
    "ai-thag5.1:2.1_0": "Some men were disgusted,",
    "ai-thag5.1:2.2_0": "seeing her dead and rotten;",
    "ai-thag5.1:2.3_0": "but sexual desire arose in me,",
    "ai-thag5.1:2.4_0": "I was as if blind to her oozing body.",
    "ai-thag5.1:3.1_0": "Quicker than the cooking of rice",
    "ai-thag5.1:3.2_0": "I left that place!",
    "ai-thag5.1:3.3_0": "Mindful and aware,",
    "ai-thag5.1:3.4_0": "then sat down to one side.",
    "ai-thag5.1:4.1_0": "Then I love the hymns,",
    "ai-thag5.1:4.2_0": "proper attention.",
    "ai-thag5.1:4.3_0": "that wise person appeared,",
    "ai-thag5.1:4.4_0": "Consider disillusionment.",
    "ai-thag5.1:5.1_0": "my mind was freed,",
    "ai-thag5.1:5.2_0": "Just one rainy season after I went forth,",
    "ai-thag5.1:5.3_0": "I attained the three knowledges",
    "ai-thag5.1:5.4_0": "having fulfilled the Buddha’s instructions.",
    "ai-thag5.1:5.5_0": "as a senior monk ..."
}