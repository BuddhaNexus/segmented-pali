{
    "ai-ja192:0.1_0": "Jātaka",
    "ai-ja192:0.2_0": "Dukanipāta",
    "ai-ja192:0.3_0": "Ruhakavagga",
    "ai-ja192:0.4_0": "2. Sirikāḷakaṇṇijātaka",
    "ai-ja192:1.1_0": "“One who has self-controlled,",
    "ai-ja192:1.2_0": "would be ethical and virtuous",
    "ai-ja192:1.3_0": "wouldn’t honor anyone,",
    "ai-ja192:1.4_0": "is pouring these Mahaka.”",
    "ai-ja192:2.1_0": "“I’m a mirror of faith, great king,",
    "ai-ja192:2.2_0": "may a man not hurt.",
    "ai-ja192:2.3_0": "your head and Kāḷa,",
    "ai-ja192:2.4_0": "like the water in tiny streams.”",
    "ai-ja192:3.1_0": "Second: With Māḷakapāna"
}