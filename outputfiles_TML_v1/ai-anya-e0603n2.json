{
    "ai-anya-e0603n2:0_0": "Uṇṇālomikanātha vandanā",
    "ai-anya-e0603n2:1_0": "held up the bundle of gold,",
    "ai-anya-e0603n2:2_0": "grew prettily between his eyebrows.",
    "ai-anya-e0603n2:3_0": "Like controlled by hunger,",
    "ai-anya-e0603n2:4_0": "who came out from the lay life to homelessness.",
    "ai-anya-e0603n2:5_0": "Like a mule of fans,",
    "ai-anya-e0603n2:6_0": "substance, beautiful, and ugly,",
    "ai-anya-e0603n2:7_0": "decayed and perish,",
    "ai-anya-e0603n2:8_0": "like a flash of lightning.",
    "ai-anya-e0603n2:9_0": "tireless all over with joy,",
    "ai-anya-e0603n2:10_0": "while eating and drinking",
    "ai-anya-e0603n2:11_0": "Disciples",
    "ai-anya-e0603n2:12_0": "decayed and perish."
}