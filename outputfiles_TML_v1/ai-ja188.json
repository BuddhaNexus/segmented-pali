{
    "ai-ja188:0.1_0": "Jātaka",
    "ai-ja188:0.2_0": "Dukanipāta",
    "ai-ja188:0.3_0": "Asadisavagga",
    "ai-ja188:0.4_0": "8. Sīhakotthujātaka",
    "ai-ja188:1.1_0": "Like a lion,",
    "ai-ja188:1.2_0": "established in the feet of the lion,",
    "ai-ja188:1.3_0": "lion, king of lions,",
    "ai-ja188:1.4_0": "but not of anyone else.”",
    "ai-ja188:2.1_0": "“Don’t let the king,",
    "ai-ja188:2.2_0": "quiet and still,",
    "ai-ja188:2.3_0": "Would you know that",
    "ai-ja188:2.4_0": "For they have no shelter.”",
    "ai-ja188:3.1_0": "The eighth sub-chapter on Sīha is finished."
}