{
    "ai-tha-ap265:0.1_0": "Therāpadāna",
    "ai-tha-ap265:0.2_0": "Padumukkhipavagga",
    "ai-tha-ap265:0.3_0": "3. Aḍḍhacandiyattheraapadāna",
    "ai-tha-ap265:1.1_0": "“One year",
    "ai-tha-ap265:1.2_0": "I went to Bodhi,",
    "ai-tha-ap265:1.3_0": "I’ve given half a month,",
    "ai-tha-ap265:1.4_0": "your foot creeper burns.",
    "ai-tha-ap265:2.1_0": "All the eons",
    "ai-tha-ap265:2.2_0": "with the Moon God.",
    "ai-tha-ap265:2.3_0": "I’m not aware of any bad places",
    "ai-tha-ap265:2.4_0": "I’ve been born in the awakening.",
    "ai-tha-ap265:3.1_0": "At five eons have passed",
    "ai-tha-ap265:3.2_0": "a king named gods,",
    "ai-tha-ap265:3.3_0": "Endowed with the seven gems,",
    "ai-tha-ap265:3.4_0": "Mahāpajāpatī Gotamī.",
    "ai-tha-ap265:4.1_0": "Attaining the four kinds of textual analysis",
    "ai-tha-ap265:4.2_0": "... “I attained the noble ones,” ...",
    "ai-tha-ap265:4.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap265:5.1_0": "That is how these verses were recited by the senior venerable Nandiya.",
    "ai-tha-ap265:6.1_0": "and a third, acting with senior monks."
}