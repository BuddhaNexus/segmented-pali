{
    "ai-tha-ap288:0.1_0": "Therāpadāna",
    "ai-tha-ap288:0.2_0": "Paṇṇadāyakavagga",
    "ai-tha-ap288:0.3_0": "Declaring his right,",
    "ai-tha-ap288:1.1_0": "“Uttarā, live mindfully,",
    "ai-tha-ap288:1.2_0": "receiver of sacrifices,",
    "ai-tha-ap288:1.3_0": "there are two great lakes,",
    "ai-tha-ap288:1.4_0": "the world’s Elder, and the world does not clever.",
    "ai-tha-ap288:2.1_0": "She went forth",
    "ai-tha-ap288:2.2_0": "the whole world.",
    "ai-tha-ap288:2.3_0": "I’ve given to him,",
    "ai-tha-ap288:2.4_0": "He taught the great hermit.",
    "ai-tha-ap288:3.1_0": "who is receiving the Buddha,",
    "ai-tha-ap288:3.2_0": "handing over the Great Hell.",
    "ai-tha-ap288:3.3_0": "and getting up early,",
    "ai-tha-ap288:3.4_0": "and he left.",
    "ai-tha-ap288:4.1_0": "More than three eons",
    "ai-tha-ap288:4.2_0": "serving them,",
    "ai-tha-ap288:4.3_0": "I’m not aware of any bad places",
    "ai-tha-ap288:4.4_0": "the fruit of the remainder.",
    "ai-tha-ap288:5.1_0": "Sentient beings have seven eons",
    "ai-tha-ap288:5.2_0": "plenty of strength,",
    "ai-tha-ap288:5.3_0": "Endowed with the seven gems,",
    "ai-tha-ap288:5.4_0": "Mahāpajāpatī Gotamī.",
    "ai-tha-ap288:6.1_0": "Attaining the four kinds of textual analysis",
    "ai-tha-ap288:6.2_0": "... “I attained the noble ones,” ...",
    "ai-tha-ap288:6.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap288:7.1_0": "That is how these verses were recited by the senior venerable Upasena.",
    "ai-tha-ap288:8.1_0": "sixth part of the senior monks who had grown."
}