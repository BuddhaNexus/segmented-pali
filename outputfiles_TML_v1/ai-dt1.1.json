{
    "ai-dt1.1:0.1_0": "Dhātukathā",
    "ai-dt1.1:0.2_0": "Uddesa",
    "ai-dt1.1:0.3_0": "1. Nayamātikā",
    "ai-dt1.1:1.1_0": "Homage to that Blessed One, the perfected one, the fully awakened Buddha!",
    "ai-dt1.1:2.1_0": "Not inclusion",
    "ai-dt1.1:3.1_0": "Accomplished in the World",
    "ai-dt1.1:4.1_0": "Accomplished in the various ways:",
    "ai-dt1.1:5.1_0": "Ways of being inclusive:",
    "ai-dt1.1:6.1_0": "Soṇa, endowed with the unattached,",
    "ai-dt1.1:7.1_0": "Happy and fatching:",
    "ai-dt1.1:8.1_0": "committed to figure,",
    "ai-dt1.1:9.1_0": "committed to figurance,",
    "ai-dt1.1:10.1_0": "Endowed with contempt,",
    "ai-dt1.1:11.1_0": "bound together with his army of fat,",
    "ai-dt1.1:12.1_0": "Delicate among men,",
    "ai-dt1.1:13.1_0": "Accomplished in the lower part of the body,",
    "ai-dt1.1:14.1_0": "Not fixed, always committed to Gotama,",
    "ai-dt1.1:15.1_0": "by inclusion, not inclusion."
}