{
    "ai-mil3.5.4:0.1_0": "Milindapañha",
    "ai-mil3.5.4:0.2_0": "Buddhavagga",
    "ai-mil3.5.4:0.3_0": "4. Dhammadiṭṭhapañha",
    "ai-mil3.5.4:1.1_0": "King Dīghīti says,",
    "ai-mil3.5.4:1.2_0": "that you saw him,",
    "ai-mil3.5.4:1.3_0": "“Great king, regarding the Buddha, the disciples of the Buddha are regarded as such.”",
    "ai-mil3.5.4:2.1_0": "“Venerable Sir, you’re buildings.”",
    "ai-mil3.5.4:3.1_0": "with joined palms."
}