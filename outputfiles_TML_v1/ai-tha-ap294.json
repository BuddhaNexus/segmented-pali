{
    "ai-tha-ap294:0.1_0": "Therāpadāna",
    "ai-tha-ap294:0.2_0": "Citakapūjakavagga",
    "ai-tha-ap294:0.3_0": "2. Pupphadhārakattheraapadāna",
    "ai-tha-ap294:1.1_0": "I was Jotika,",
    "ai-tha-ap294:1.2_0": "with unskilled in the path,",
    "ai-tha-ap294:1.3_0": "by direct knowledge,",
    "ai-tha-ap294:1.4_0": "like the Moon God.",
    "ai-tha-ap294:2.1_0": "Seeing the light of the world,",
    "ai-tha-ap294:2.2_0": "with a humble One—",
    "ai-tha-ap294:2.3_0": "flower’s with flowers",
    "ai-tha-ap294:2.4_0": "I remembered the Teacher’s words.",
    "ai-tha-ap294:3.1_0": "At most eons",
    "ai-tha-ap294:3.2_0": "flowering along with flowers,",
    "ai-tha-ap294:3.3_0": "I’m not aware of any bad places",
    "ai-tha-ap294:3.4_0": "that is the fruit of the ascetic.",
    "ai-tha-ap294:4.1_0": "At seven days, then,",
    "ai-tha-ap294:4.2_0": "one is queen,",
    "ai-tha-ap294:4.3_0": "There is a hall named Vāsava,",
    "ai-tha-ap294:4.4_0": "Mahāpajāpatī Gotamī.",
    "ai-tha-ap294:5.1_0": "Attaining the four kinds of textual analysis",
    "ai-tha-ap294:5.2_0": "... “I attained the noble ones,” ...",
    "ai-tha-ap294:5.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap294:6.1_0": "That is how these verses were recited by the senior venerable Bodhī.",
    "ai-tha-ap294:7.1_0": "by gardening a flower,"
}