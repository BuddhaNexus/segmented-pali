{
    "ai-ja207:0.1_0": "Jātaka",
    "ai-ja207:0.2_0": "Dukanipāta",
    "ai-ja207:0.3_0": "Nataṁdaḷhavagga",
    "ai-ja207:0.4_0": "7. Assakajātaka",
    "ai-ja207:1.1_0": "The King of Kosala",
    "ai-ja207:1.2_0": "I’ve taught this Dhamma.",
    "ai-ja207:1.3_0": "It’s just because I’m accidentally",
    "ai-ja207:1.4_0": "with husband.",
    "ai-ja207:2.1_0": "Though pleasure and pain,",
    "ai-ja207:2.2_0": "for the origin of the world,",
    "ai-ja207:2.3_0": "That’s why a wise one would stop",
    "ai-ja207:2.4_0": "alone is my loved ones.”",
    "ai-ja207:3.1_0": "on the seventh side, the horse meat."
}