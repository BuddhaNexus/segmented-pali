{
    "ai-ja38:0.1_0": "Jātaka",
    "ai-ja38:0.2_0": "Ekakanipāta",
    "ai-ja38:0.3_0": "Kulāvakavagga",
    "ai-ja38:0.4_0": "8. Bakajātaka",
    "ai-ja38:1.1_0": "“You’re alert, mindful,",
    "ai-ja38:1.2_0": "what you call happiness.",
    "ai-ja38:1.3_0": "They’re witless, dull, and stupid.",
    "ai-ja38:1.4_0": "I am Kakkaṭa.”",
    "ai-ja38:2.1_0": "The eighth sub-chapter on arms akimbo."
}