{
    "ai-thag1.50:0.1_0": "Verses of the Senior Monks",
    "ai-thag1.50:0.2_0": "The Book of the Ones",
    "ai-thag1.50:0.3_0": "Chapter Five",
    "ai-thag1.50:0.4_0": "1.50. Vimala (1st)",
    "ai-thag1.50:1.1_0": "The rain falls and the wind blows on mother earth,",
    "ai-thag1.50:1.2_0": "while lightning flashes across the sky!",
    "ai-thag1.50:1.3_0": "But my thoughts are stilled,",
    "ai-thag1.50:1.4_0": "my mind is serene.",
    "ai-thag1.50:1.5_0": "…",
    "ai-thag1.50:1.6_0": "a senior monk ...",
    "ai-thag1.50:1.7_0": "the fifth and graceful.",
    "ai-thag1.50:2.0_0": "Inspiring places",
    "ai-thag1.50:2.1_0": "Revata the senior monk Sirivaḍḍha,",
    "ai-thag1.50:2.2_0": "plague bolts with blue net,",
    "ai-thag1.50:2.3_0": "Ramaṇīyavihārin",
    "ai-thag1.50:2.4_0": "is filled with joined palms.",
    "ai-thag1.50:2.5_0": "The senior monk Soṇī",
    "ai-thag1.50:2.6_0": "palace, you prevail over discontent."
}