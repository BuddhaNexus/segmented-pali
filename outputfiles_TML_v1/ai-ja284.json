{
    "ai-ja284:0.1_0": "Jātaka",
    "ai-ja284:0.2_0": "Tikanipāta",
    "ai-ja284:0.3_0": "Abbhantaravagga",
    "ai-ja284:0.4_0": "4. Sirijātaka",
    "ai-ja284:1.1_0": "“This is the hair to look on it;",
    "ai-ja284:1.2_0": "money and grain.",
    "ai-ja284:1.3_0": "craft, craft,",
    "ai-ja284:1.4_0": "he eats the food that has been received.",
    "ai-ja284:2.1_0": "having done good,",
    "ai-ja284:2.2_0": "they kill living creatures;",
    "ai-ja284:2.3_0": "What wealth arises,",
    "ai-ja284:2.4_0": "among the sense fields,",
    "ai-ja284:3.1_0": "Chickens, leaning on a staff,",
    "ai-ja284:3.2_0": "and reconciled in merit.",
    "ai-ja284:3.3_0": "“The mind leads to quenching,",
    "ai-ja284:3.4_0": "who has done good things.”",
    "ai-ja284:4.1_0": "on the fourth part of the body."
}