{
    "ai-ja247:0.1_0": "Jātaka",
    "ai-ja247:0.2_0": "Dukanipāta",
    "ai-ja247:0.3_0": "Siṅgālavagga",
    "ai-ja247:0.4_0": "7. Pādañjalījātaka",
    "ai-ja247:1.1_0": "“Clearly, consummate,",
    "ai-ja247:1.2_0": "outshines with their wisdom.",
    "ai-ja247:1.3_0": "That’s how, sang;",
    "ai-ja247:1.4_0": "surely I’ve seen someone else who sees this.",
    "ai-ja247:2.1_0": "“One who teaches Dhamma",
    "ai-ja247:2.2_0": "and you penetrate the goal.",
    "ai-ja247:2.3_0": "except the cow",
    "ai-ja247:2.4_0": "but for what is unskilled.”",
    "ai-ja247:3.1_0": "the seventh ointment boxes."
}