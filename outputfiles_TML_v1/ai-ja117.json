{
    "ai-ja117:0.1_0": "Jātaka",
    "ai-ja117:0.2_0": "Ekakanipāta",
    "ai-ja117:0.3_0": "Haṁcivagga",
    "ai-ja117:0.4_0": "7. Tittirajātaka",
    "ai-ja117:1.1_0": "“Overcoming to correct,",
    "ai-ja117:1.2_0": "spending too much time informing,",
    "ai-ja117:1.3_0": "speech, and mind are hard to admonish,",
    "ai-ja117:1.4_0": "you’ll stay like a jackal.”",
    "ai-ja117:2.1_0": "Thirty-Three was the seventh, the wife of mine."
}