{
    "ai-thag1.18:0.1_0": "Verses of the Senior Monks",
    "ai-thag1.18:0.2_0": "The Book of the Ones",
    "ai-thag1.18:0.3_0": "Chapter Two",
    "ai-thag1.18:0.4_0": "1.18. Siṅgāla’s Father",
    "ai-thag1.18:1.1_0": "There was an heir of the Buddha,",
    "ai-thag1.18:1.2_0": "a monk in Bhesakaḷā forest,",
    "ai-thag1.18:1.3_0": "who suffused the entire earth",
    "ai-thag1.18:1.4_0": "with the perception of bones.",
    "ai-thag1.18:1.5_0": "I think he will quickly",
    "ai-thag1.18:1.6_0": "get rid of sensual desire.",
    "ai-thag1.18:1.7_0": "... who are senior monk of Yuna."
}