{
    "ai-thag1.16:0.1_0": "Verses of the Senior Monks",
    "ai-thag1.16:0.2_0": "The Book of the Ones",
    "ai-thag1.16:0.3_0": "Chapter Two",
    "ai-thag1.16:0.4_0": "1.16. Belaṭṭhasīsa",
    "ai-thag1.16:1.1_0": "Just as a fine thoroughbred",
    "ai-thag1.16:1.2_0": "proceeds with ease,",
    "ai-thag1.16:1.3_0": "tail and mane flying in the wind;",
    "ai-thag1.16:1.4_0": "so my days and nights",
    "ai-thag1.16:1.5_0": "proceed with ease,",
    "ai-thag1.16:1.6_0": "full of spiritual joy.",
    "ai-thag1.16:1.7_0": "Belaṭṭhasīsa,"
}