{
    "ai-ja211:0.1_0": "Jātaka",
    "ai-ja211:0.2_0": "Dukanipāta",
    "ai-ja211:0.3_0": "Bīraṇathambhavagga",
    "ai-ja211:0.4_0": "1. Somadattajātaka",
    "ai-ja211:1.1_0": "“Formerly there is much water,",
    "ai-ja211:1.2_0": "the food and drinks of fear;",
    "ai-ja211:1.3_0": "restrained, with perception—",
    "ai-ja211:1.4_0": "is not devoted to this.”",
    "ai-ja211:2.1_0": "“As a little bit, my dear,",
    "ai-ja211:2.2_0": "finds out.",
    "ai-ja211:2.3_0": "loss, wealth, or health;",
    "ai-ja211:2.4_0": "so thoughts.”",
    "ai-ja211:3.1_0": "He was the first born of seven people."
}