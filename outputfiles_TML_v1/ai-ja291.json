{
    "ai-ja291:0.1_0": "Jātaka",
    "ai-ja291:0.2_0": "Tikanipāta",
    "ai-ja291:0.3_0": "Kumbhavagga",
    "ai-ja291:0.4_0": "1. Surāghaṭajātaka",
    "ai-ja291:1.1_0": "I’ve plucked out the pots of rice,",
    "ai-ja291:1.2_0": "plucky of ties,",
    "ai-ja291:1.3_0": "As long as they walk",
    "ai-ja291:1.4_0": "they prospers in happiness.",
    "ai-ja291:2.1_0": "But when I’m dead, fearing,",
    "ai-ja291:2.2_0": "negligence, potters,",
    "ai-ja291:2.3_0": "And a naked one and jute canvas,",
    "ai-ja291:2.4_0": "the fool frets.",
    "ai-ja291:3.1_0": "In the same way, there’s wealth,",
    "ai-ja291:3.2_0": "enjoys it, he eats it;",
    "ai-ja291:3.3_0": "Afterwards the fool",
    "ai-ja291:3.4_0": "like a plucky broom.”",
    "ai-ja291:4.1_0": "first from the house."
}