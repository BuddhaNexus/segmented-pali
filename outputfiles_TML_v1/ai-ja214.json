{
    "ai-ja214:0.1_0": "Jātaka",
    "ai-ja214:0.2_0": "Dukanipāta",
    "ai-ja214:0.3_0": "Bīraṇathambhavagga",
    "ai-ja214:0.4_0": "4. Puṇṇanadījātaka",
    "ai-ja214:1.1_0": "“Puṇṇaka, whis,",
    "ai-ja214:1.2_0": "and secrets for you.",
    "ai-ja214:1.3_0": "They carcas fall,",
    "ai-ja214:1.4_0": "Let’s eat, brahmin, and have a meal of brahmins.”",
    "ai-ja214:2.1_0": "When I remember my king,",
    "ai-ja214:2.2_0": "or even if they send you away.",
    "ai-ja214:2.3_0": "“Gese, herons, and peacocks,",
    "ai-ja214:2.4_0": "but none of them.”",
    "ai-ja214:3.1_0": "with a fourth born from wall."
}