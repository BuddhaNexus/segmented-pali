{
    "ai-tha-ap379:0.1_0": "Therāpadāna",
    "ai-tha-ap379:0.2_0": "Bodhivandanavagga",
    "ai-tha-ap379:0.3_0": "7. Citakapūjakattheraapadāna",
    "ai-tha-ap379:1.1_0": "“The Buddha has become fully extinguished",
    "ai-tha-ap379:1.2_0": "and lord of old age.",
    "ai-tha-ap379:1.3_0": "Roja experienced this with crawling",
    "ai-tha-ap379:1.4_0": "flowering on a garden,",
    "ai-tha-ap379:2.1_0": "A hundred thousand eons",
    "ai-tha-ap379:2.2_0": "flowering along with flowers,",
    "ai-tha-ap379:2.3_0": "I’m not aware of any bad places",
    "ai-tha-ap379:2.4_0": "full of different fruits.",
    "ai-tha-ap379:3.1_0": "My defilements have been burnt away by practicing absorption—",
    "ai-tha-ap379:3.2_0": "... “I attained the noble ones,” ...",
    "ai-tha-ap379:3.3_0": "and live without defilements.",
    "ai-tha-ap379:4.1_0": "It was truly for me",
    "ai-tha-ap379:4.2_0": "... “I attained the noble ones,” ...",
    "ai-tha-ap379:4.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap379:5.1_0": "Attaining the four kinds of textual analysis",
    "ai-tha-ap379:5.2_0": "... “I attained the noble ones,” ...",
    "ai-tha-ap379:5.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap379:6.1_0": "That is how these verses were recited by the senior venerable Bhūmajaka.",
    "ai-tha-ap379:7.1_0": "a quail, with senior monks who had gone forth."
}