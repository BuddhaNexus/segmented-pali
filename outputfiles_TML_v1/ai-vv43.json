{
    "ai-vv43:0.1_0": "Vimānavatthu",
    "ai-vv43:0.2_0": "Itthivimāna",
    "ai-vv43:0.3_0": "Mañjiṭṭhakavagga",
    "ai-vv43:0.4_0": "5. Kañjikadāyikāvimānavatthu",
    "ai-vv43:1.1_0": "“Any man by overexerting,",
    "ai-vv43:1.2_0": "... “I attained the noble ones,” ...",
    "ai-vv43:1.3_0": "and ozing for such a one.",
    "ai-vv43:2.1_0": "Why did you praise them?",
    "ai-vv43:2.2_0": "... “I attained the noble ones,” ...",
    "ai-vv43:2.3_0": "your beauty shines like the quarter.”",
    "ai-vv43:3.1_0": "Delighted by the deities,",
    "ai-vv43:3.2_0": "... “I attained the noble ones,” ...",
    "ai-vv43:3.3_0": "that is the fruit of deeds.",
    "ai-vv43:4.1_0": "I’m blind, with dung,",
    "ai-vv43:4.2_0": "on the path that leads to quenching,",
    "ai-vv43:4.3_0": "I gave gifts to Kapilavatthu,",
    "ai-vv43:4.4_0": "ordinary oil.",
    "ai-vv43:5.1_0": "by means of fruit,",
    "ai-vv43:5.2_0": "And with the barley and the ointment boxes.",
    "ai-vv43:5.3_0": "to whom you gave upright ones,",
    "ai-vv43:5.4_0": "with a clear and confident heart.",
    "ai-vv43:6.1_0": "I’ll build one bridge,",
    "ai-vv43:6.2_0": "of a Wheel-Turning Monarch,",
    "ai-vv43:6.3_0": "they’re choked by all good qualities—",
    "ai-vv43:6.4_0": "seeing her husband and wife,",
    "ai-vv43:6.5_0": "my ointment,",
    "ai-vv43:6.6_0": "isn’t worth a sixteenth of that.",
    "ai-vv43:7.1_0": "the being soiled by mere mindfulness,",
    "ai-vv43:7.2_0": "A hundred carriages drawn by mules,",
    "ai-vv43:7.3_0": "A hundred thousand girls",
    "ai-vv43:7.4_0": "Wearing jeweled earrings",
    "ai-vv43:7.5_0": "my ointment,",
    "ai-vv43:7.6_0": "of a delightful human being.",
    "ai-vv43:8.1_0": "These each of the great elephants,",
    "ai-vv43:8.2_0": "until its neck is excellent,",
    "ai-vv43:8.3_0": "a golden garland,",
    "ai-vv43:8.4_0": "put in charge of building work,",
    "ai-vv43:8.5_0": "my ointment,",
    "ai-vv43:8.6_0": "of a delightful human being.",
    "ai-vv43:9.1_0": "I gained among the gods,",
    "ai-vv43:9.2_0": "slandering the hermits,",
    "ai-vv43:9.3_0": "my ointment,",
    "ai-vv43:9.4_0": "of the happiness of blamelessness.”",
    "ai-vv43:10.1_0": "Five Discourses on creams,"
}