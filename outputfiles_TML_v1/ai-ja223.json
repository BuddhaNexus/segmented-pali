{
    "ai-ja223:0.1_0": "Jātaka",
    "ai-ja223:0.2_0": "Dukanipāta",
    "ai-ja223:0.3_0": "Kāsāvavagga",
    "ai-ja223:0.4_0": "3. Puṭabhattajātaka",
    "ai-ja223:1.1_0": "“Don’t associate with such a person,",
    "ai-ja223:1.2_0": "with one whose duty is guarded;",
    "ai-ja223:1.3_0": "for the meditation to understand,",
    "ai-ja223:1.4_0": "undiscipled, can’t become his equal.",
    "ai-ja223:2.1_0": "Steadying in the forest,",
    "ai-ja223:2.2_0": "lack of proper reflection,",
    "ai-ja223:2.3_0": "Knowing the fruit of deeds,",
    "ai-ja223:2.4_0": "all the world is large.”",
    "ai-ja223:3.1_0": "and third-born on the third part."
}