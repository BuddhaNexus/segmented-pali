{
    "ai-ja41:0.1_0": "Jātaka",
    "ai-ja41:0.2_0": "Ekakanipāta",
    "ai-ja41:0.3_0": "Atthakāmavagga",
    "ai-ja41:0.4_0": "1. Losakajātaka",
    "ai-ja41:1.1_0": "“Anyone who loves the teaching",
    "ai-ja41:1.2_0": "while giving,",
    "ai-ja41:1.3_0": "Sumedhā, lacking a foot,",
    "ai-ja41:1.4_0": "you shine among your friends.”",
    "ai-ja41:2.1_0": "first from the world of the loads."
}