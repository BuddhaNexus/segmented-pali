{
    "ai-thi-ap40:0.1_0": "Therīapadāna",
    "ai-thi-ap40:0.2_0": "Khattiyāvagga",
    "ai-thi-ap40:0.3_0": "10 Pesalātherīapadāna",
    "ai-thi-ap40:1.1_0": "“Whose merit is Bhaddā,",
    "ai-thi-ap40:1.2_0": "they’re caught in Brahmā,",
    "ai-thi-ap40:1.3_0": "Kassapagotta",
    "ai-thi-ap40:1.4_0": "given rise to sorrow.",
