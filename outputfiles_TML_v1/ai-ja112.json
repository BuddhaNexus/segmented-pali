{
    "ai-ja112:0.1_0": "Jātaka",
    "ai-ja112:0.2_0": "Ekakanipāta",
    "ai-ja112:0.3_0": "Haṁcivagga",
    "ai-ja112:0.4_0": "2. Amarādevīpañhajātaka",
    "ai-ja112:1.1_0": "“Whoever has a hundred riverbank,",
    "ai-ja112:1.2_0": "flowers, with flowers,",
    "ai-ja112:1.3_0": "I say that there is giving,",
    "ai-ja112:1.4_0": "I don’t say that anyone who gives it up.",
    "ai-ja112:1.5_0": "This path is the straight path,",
    "ai-ja112:1.6_0": "Let us understand this path.”",
    "ai-ja112:2.1_0": "and thought, “You don’t know moderation.”"
}