{
    "ai-ja225:0.1_0": "Jātaka",
    "ai-ja225:0.2_0": "Dukanipāta",
    "ai-ja225:0.3_0": "Kāsāvavagga",
    "ai-ja225:0.4_0": "5. Khantivaṇṇajātaka",
    "ai-ja225:1.1_0": "“I have, sir,",
    "ai-ja225:1.2_0": "and won’t throat out,",
    "ai-ja225:1.3_0": "At the next time there is no shelter,",
    "ai-ja225:1.4_0": "In that case, you have done so?”",
    "ai-ja225:2.1_0": "“We went back to Campā,",
    "ai-ja225:2.2_0": "Such is Truth,",
    "ai-ja225:2.3_0": "Accomplished in the teaching,",
    "ai-ja225:2.4_0": "Wanting, important.”",
    "ai-ja225:3.1_0": "and the fifth."
}