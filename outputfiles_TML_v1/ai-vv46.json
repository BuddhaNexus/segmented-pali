{
    "ai-vv46:0.1_0": "Vimānavatthu",
    "ai-vv46:0.2_0": "Itthivimāna",
    "ai-vv46:0.3_0": "Mañjiṭṭhakavagga",
    "ai-vv46:0.4_0": "8. Ambavimānavatthu",
    "ai-vv46:1.1_0": "“I saw the mango grove,",
    "ai-vv46:1.2_0": "at a stilt longhouse and a large.",
    "ai-vv46:1.3_0": "corrupted by many arms,",
    "ai-vv46:1.4_0": "and monsters made a voice.",
    "ai-vv46:2.1_0": "Like a lamp burning there,",
    "ai-vv46:2.2_0": "always love",
    "ai-vv46:2.3_0": "Setle down by a tree,",
    "ai-vv46:2.4_0": "than surrounded.",
    "ai-vv46:3.1_0": "Why did you praise them?",
    "ai-vv46:3.2_0": "... “I attained the noble ones,” ...",
    "ai-vv46:3.3_0": "your beauty shines like the quarter.”",
    "ai-vv46:4.1_0": "Delighted by the deities,",
    "ai-vv46:4.2_0": "... “I attained the noble ones,” ...",
    "ai-vv46:4.3_0": "that is the fruit of deeds.",
    "ai-vv46:5.1_0": "I am human beings,",
    "ai-vv46:5.2_0": "for the sake of birth and death.",
    "ai-vv46:5.3_0": "I entered a dwelling for the Sangha,",
    "ai-vv46:5.4_0": "mangoes.",
    "ai-vv46:6.1_0": "and he has gone to the dwelling,",
    "ai-vv46:6.2_0": "I am reckoned as a god,",
    "ai-vv46:6.3_0": "I’ve cut off the mango grove,",
    "ai-vv46:6.4_0": "having cut the cloth in a piece of fruit.",
    "ai-vv46:7.1_0": "a lotus might blossom,",
    "ai-vv46:7.2_0": "I was struck with a sense of urgency.",
    "ai-vv46:7.3_0": "I taught the Sangha,",
    "ai-vv46:7.4_0": "Because they killed living creatures.",
    "ai-vv46:8.1_0": "the mango grove at the mango grove,",
    "ai-vv46:8.2_0": "at a stilt longhouse and a large.",
    "ai-vv46:8.3_0": "corrupted by many arms,",
    "ai-vv46:8.4_0": "and monsters made a voice.",
    "ai-vv46:9.1_0": "Like a lamp burning there,",
    "ai-vv46:9.2_0": "always love",
    "ai-vv46:9.3_0": "Setle down by a tree,",
    "ai-vv46:9.4_0": "than surrounded.",
    "ai-vv46:10.1_0": "With my appearance of me,",
    "ai-vv46:10.2_0": "... “I attained the noble ones,” ...",
    "ai-vv46:10.3_0": "My self-wokes like the sky.",
    "ai-vv46:11.1_0": "the eighth mangoes."
}