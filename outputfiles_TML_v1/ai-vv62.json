{
    "ai-vv62:0.1_0": "Vimānavatthu",
    "ai-vv62:0.2_0": "Purisavimāna",
    "ai-vv62:0.3_0": "Mahārathavagga",
    "ai-vv62:0.4_0": "12. Tatiyanāgavimānavatthu",
    "ai-vv62:1.1_0": "“What difference does it make—",
    "ai-vv62:1.2_0": "the bond of all elephants,",
    "ai-vv62:1.3_0": "Taking arrows,",
    "ai-vv62:1.4_0": "Veakkhali,",
    "ai-vv62:2.1_0": "So you should fairy,",
    "ai-vv62:2.2_0": "known as Vāsava, the first of the Vasus.",
    "ai-vv62:2.3_0": "Not knowing, I ask—",
    "ai-vv62:2.4_0": "how am I to know who you are?”",
    "ai-vv62:3.1_0": "“I’m no god nor fairy,",
    "ai-vv62:3.2_0": "didn’t lord of men.",
    "ai-vv62:3.3_0": "Those gods who are well proclaimed,",
    "ai-vv62:3.4_0": "a certain snake.”",
    "ai-vv62:4.1_0": "“I ask you about, sir,",
    "ai-vv62:4.2_0": "having joined palms raised to him:",
    "ai-vv62:4.3_0": "What did deed have done to human deeds,",
    "ai-vv62:4.4_0": "he’s reborn in a good place.”",
    "ai-vv62:5.1_0": "“As a straw mat,",
    "ai-vv62:5.2_0": "clothes, and giving.",
    "ai-vv62:5.3_0": "Having given a certain night,",
    "ai-vv62:5.4_0": "he’s reborn in a good place.”",
    "ai-vv62:6.1_0": "Third Absorption."
}