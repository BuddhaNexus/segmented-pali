{
    "ai-ja8:0.1_0": "Jātaka",
    "ai-ja8:0.2_0": "Ekakanipāta",
    "ai-ja8:0.3_0": "Apaṇṇakavagga",
    "ai-ja8:0.4_0": "8. Gāmaṇijātaka",
    "ai-ja8:1.1_0": "“In the midst of the Buddha,",
    "ai-ja8:1.2_0": "the fruit falls off,",
    "ai-ja8:1.3_0": "The spiritual life is lived",
    "ai-ja8:1.4_0": "a village in the neighborhood of a village.”",
    "ai-ja8:2.1_0": "eighth of the chief."
}