{
    "ai-ja127:0.1_0": "Jātaka",
    "ai-ja127:0.2_0": "Ekakanipāta",
    "ai-ja127:0.3_0": "Kusanāḷivagga",
    "ai-ja127:0.4_0": "7. Kalaṇḍukajātaka",
    "ai-ja127:1.1_0": "“Ascetics, worldly pleasures",
    "ai-ja127:1.2_0": "and I plunged into the forest.",
    "ai-ja127:1.3_0": "Would renown",
    "ai-ja127:1.4_0": "like a carpenter.”",
    "ai-ja127:2.1_0": "ready to grow old."
}