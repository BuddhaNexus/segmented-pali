{
    "ai-tha-ap437:0.1_0": "Therāpadāna",
    "ai-tha-ap437:0.2_0": "Ekavihārivagga",
    "ai-tha-ap437:0.3_0": "5. Ucchukhaṇḍikattheraapadāna",
    "ai-tha-ap437:1.1_0": "I’ve gone mad,",
    "ai-tha-ap437:1.2_0": "an open gate,",
    "ai-tha-ap437:1.3_0": "I saw the stainless Buddha",
    "ai-tha-ap437:1.4_0": "who has gone beyond all things.",
    "ai-tha-ap437:2.1_0": "Has eaten this by the rod,",
    "ai-tha-ap437:2.2_0": "by the best of Buddhas,",
    "ai-tha-ap437:2.3_0": "confident and settled,",
    "ai-tha-ap437:2.4_0": "he was the great hermits.",
    "ai-tha-ap437:3.1_0": "At most eons",
    "ai-tha-ap437:3.2_0": "I sugarded this.",
    "ai-tha-ap437:3.3_0": "I’m not aware of any bad places",
    "ai-tha-ap437:3.4_0": "sugarcane fruit.",
    "ai-tha-ap437:4.1_0": "My defilements have been burnt away by practicing absorption—",
    "ai-tha-ap437:4.2_0": "... “I attained the noble ones,” ...",
    "ai-tha-ap437:4.3_0": "and live without defilements.",
    "ai-tha-ap437:5.1_0": "It was truly for me",
    "ai-tha-ap437:5.2_0": "... “I attained the noble ones,” ...",
    "ai-tha-ap437:5.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap437:6.1_0": "Attaining the four kinds of textual analysis",
    "ai-tha-ap437:6.2_0": "... “I attained the noble ones,” ...",
    "ai-tha-ap437:6.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap437:7.1_0": "That is how these verses were recited by the senior venerable Ukkaṭṭhā.",
    "ai-tha-ap437:8.1_0": "fine rice with the fifth part of the senior monks."
}