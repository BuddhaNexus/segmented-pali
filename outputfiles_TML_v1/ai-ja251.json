{
    "ai-ja251:0.1_0": "Jātaka",
    "ai-ja251:0.2_0": "Tikanipāta",
    "ai-ja251:0.3_0": "Saṅkappavagga",
    "ai-ja251:0.4_0": "1. Saṅkapparāgajātaka",
    "ai-ja251:1.1_0": "“For the bones without greed,",
    "ai-ja251:1.2_0": "while still thought,",
    "ai-ja251:1.3_0": "the one with a fine thoroughbred,",
    "ai-ja251:1.4_0": "And with arrows,",
    "ai-ja251:2.1_0": "freed by ear,",
    "ai-ja251:2.2_0": "nor do I the best of men.",
    "ai-ja251:2.3_0": "I’ve stired my heart",
    "ai-ja251:2.4_0": "are completely understood.",
    "ai-ja251:3.1_0": "I don’t see anyone,",
    "ai-ja251:3.2_0": "since you are satisfied,",
    "ai-ja251:3.3_0": "until his mind is unstained,",
    "ai-ja251:3.4_0": "I’ve consulted suffering.”",
    "ai-ja251:4.1_0": "Greed is the first thing."
}