{
    "ai-tha-ap386:0.1_0": "Therāpadāna",
    "ai-tha-ap386:0.2_0": "Avaṭaphalavagga",
    "ai-tha-ap386:0.3_0": "The Elder Nun Place Leader of Fruits",
    "ai-tha-ap386:1.1_0": "“Having seen the forest, Buddha,",
    "ai-tha-ap386:1.2_0": "I saw a large sacrifice.",
    "ai-tha-ap386:1.3_0": "confident in heart,",
    "ai-tha-ap386:1.4_0": "I’ll beat the fruit,",
    "ai-tha-ap386:2.1_0": "There are eighteen eons",
    "ai-tha-ap386:2.2_0": "I used to be a fruit.",
    "ai-tha-ap386:2.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap386:2.4_0": "as fruit is the fruit.",
    "ai-tha-ap386:3.1_0": "corrupted by defilements,",
    "ai-tha-ap386:3.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap386:3.3_0": "and live without defilements.",
    "ai-tha-ap386:4.1_0": "It was so welcome for me",
    "ai-tha-ap386:4.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap386:4.3_0": "and fulfilled the Buddha’s instructions.",
    "ai-tha-ap386:5.1_0": "the four analytical knowledges,",
    "ai-tha-ap386:5.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap386:5.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap386:6.1_0": "That is how these verses were recited by the senior venerable Piṅgiyānī.",
    "ai-tha-ap386:7.1_0": "for the fourth Elder Piṇḍola Bhāradvāja"
}