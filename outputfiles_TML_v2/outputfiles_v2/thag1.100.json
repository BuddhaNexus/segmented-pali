{
    "ai-thag1.100:0.1_0": "Theragāthā",
    "ai-thag1.100:0.2_0": "Ekakanipāta",
    "ai-thag1.100:0.3_0": "Dasamavagga",
    "ai-thag1.100:0.4_0": "Second: Devasabha",
    "ai-thag1.100:1.1_0": "Accomplished in the four right efforts,",
    "ai-thag1.100:1.2_0": "mindfulness meditation is your territory;",
    "ai-thag1.100:1.3_0": "festooned with the flowers of liberation,",
    "ai-thag1.100:1.4_0": "will be quenched without defilements.",
    "ai-thag1.100:1.5_0": "…",
    "ai-thag1.100:1.6_0": "They’re senior,",
    "ai-thag1.100:1.7_0": "The tenth sub-chapter on lying down is finished.",
    "ai-thag1.100:2.0_0": "“This is the summary:",
    "ai-thag1.100:2.1_0": "full of pure strength,",
    "ai-thag1.100:2.2_0": "Erāpathas I love.",
    "ai-thag1.100:2.3_0": "the seer in the eyes",
    "ai-thag1.100:2.4_0": "And three, likewise,",
    "ai-thag1.100:2.5_0": "I have great wisdom,",
    "ai-thag1.100:2.6_0": "And the elder God’s senior."
}