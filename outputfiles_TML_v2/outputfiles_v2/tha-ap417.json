{
    "ai-tha-ap417:0.1_0": "Therāpadāna",
    "ai-tha-ap417:0.2_0": "Bhaddālivagga",
    "ai-tha-ap417:0.3_0": "5 Nāgapallavattheraapadāna",
    "ai-tha-ap417:1.1_0": "“You’re not bound with a harem,",
    "ai-tha-ap417:1.2_0": "I’ll dwell in the royal compound.",
    "ai-tha-ap417:1.3_0": "covered in my hermitage.",
    "ai-tha-ap417:1.4_0": "and sat in the world.",
    "ai-tha-ap417:2.1_0": "the great dragon named Bhaddavatī.",
    "ai-tha-ap417:2.2_0": "I enjoyed the Buddha.",
    "ai-tha-ap417:2.3_0": "confident in heart,",
    "ai-tha-ap417:2.4_0": "bowing down to the Holy One,",
    "ai-tha-ap417:3.1_0": "A single eon irrevers:",
    "ai-tha-ap417:3.2_0": "I wounded the auspicious bird",
    "ai-tha-ap417:3.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap417:3.4_0": "this fruit is to know,",
    "ai-tha-ap417:4.1_0": "corrupted by defilements,",
    "ai-tha-ap417:4.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap417:4.3_0": "and live without defilements.",
    "ai-tha-ap417:5.1_0": "It was so welcome for me",
    "ai-tha-ap417:5.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap417:5.3_0": "and fulfilled the Buddha’s instructions.",
    "ai-tha-ap417:6.1_0": "the four analytical knowledges,",
    "ai-tha-ap417:6.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap417:6.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap417:7.1_0": "That is how these verses were recited by the senior venerable Nāgas.",
    "ai-tha-ap417:8.1_0": "about the fifth Elder Nāgas of the Yakkhas."
}