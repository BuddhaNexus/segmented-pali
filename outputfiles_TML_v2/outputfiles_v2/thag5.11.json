{
    "ai-thag5.11:0.1_0": "Theragāthā",
    "ai-thag5.11:0.2_0": "Pañcakanipāta",
    "ai-thag5.11:0.3_0": "Paṭhamavagga",
    "ai-thag5.11:0.4_0": "Soṇa Kuṭikaṇṇa",
    "ai-thag5.11:1.1_0": "I’ve received ordination;",
    "ai-thag5.11:1.2_0": "I’m free from defilements.",
    "ai-thag5.11:1.3_0": "But I have seen the Fortunate One,",
    "ai-thag5.11:1.4_0": "and even stayed together with him.",
    "ai-thag5.11:2.1_0": "The Blessed One, the teacher,",
    "ai-thag5.11:2.2_0": "spent much of the night in the open;",
    "ai-thag5.11:2.3_0": "then he, who is so skilled in meditation,",
    "ai-thag5.11:2.4_0": "entered his dwelling.",
    "ai-thag5.11:3.1_0": "Spreading his outer robe,",
    "ai-thag5.11:3.2_0": "he lies down.",
    "ai-thag5.11:3.3_0": "like a lion in a rocky cave,",
    "ai-thag5.11:3.4_0": "with fear and dread given up.",
    "ai-thag5.11:4.1_0": "Then, with lovely enunciation,",
    "ai-thag5.11:4.2_0": "Soṇa, a disciple of the Buddha,",
    "ai-thag5.11:4.3_0": "recited the true teaching",
    "ai-thag5.11:4.4_0": "before the best of Buddhas.",
    "ai-thag5.11:5.1_0": "Completely understanding the five aggregates,",
    "ai-thag5.11:5.2_0": "developed the direct route,",
    "ai-thag5.11:5.3_0": "having arrived at ultimate peace—",
    "ai-thag5.11:5.4_0": "will be quenched without defilements.",
    "ai-thag5.11:5.5_0": "Soṇa Kuṭikaṇṇa"
}