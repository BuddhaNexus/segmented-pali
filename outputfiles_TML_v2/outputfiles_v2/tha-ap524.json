{
    "ai-tha-ap524:0.1_0": "Therāpadāna",
    "ai-tha-ap524:0.2_0": "Tiṇadāyakavagga",
    "ai-tha-ap524:0.3_0": "2 Mañcadāyakattheraapadāna",
    "ai-tha-ap524:1.1_0": "“Look at the Fortunate One’s Tapassī,",
    "ai-tha-ap524:1.2_0": "poised one in the world.",
    "ai-tha-ap524:1.3_0": "I’ve given a single bitter",
    "ai-tha-ap524:1.4_0": "A person whom you swept away",
    "ai-tha-ap524:2.1_0": "provided with vehicles,",
    "ai-tha-ap524:2.2_0": "you’re truly for the divine.",
    "ai-tha-ap524:2.3_0": "The mind at this time",
    "ai-tha-ap524:2.4_0": "they’ve attained the ending of defilements.",
    "ai-tha-ap524:3.1_0": "A single eon irrevers:",
    "ai-tha-ap524:3.2_0": "When I (given) gave a morsel,",
    "ai-tha-ap524:3.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap524:3.4_0": "as is the fruit for a cotton-down.",
    "ai-tha-ap524:4.1_0": "corrupted by defilements,",
    "ai-tha-ap524:4.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap524:4.3_0": "and live without defilements.",
    "ai-tha-ap524:5.1_0": "It was so welcome for me",
    "ai-tha-ap524:5.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap524:5.3_0": "and fulfilled the Buddha’s instructions.",
    "ai-tha-ap524:6.1_0": "the four analytical knowledges,",
    "ai-tha-ap524:6.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap524:6.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap524:7.1_0": "That is how these verses were recited by the senior venerable Moggaliputta.",
    "ai-tha-ap524:8.1_0": "A second (work) for the Elder Divinities’ (Tree)"
}