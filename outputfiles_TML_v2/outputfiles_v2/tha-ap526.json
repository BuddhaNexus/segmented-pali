{
    "ai-tha-ap526:0.1_0": "Therāpadāna",
    "ai-tha-ap526:0.2_0": "Tiṇadāyakavagga",
    "ai-tha-ap526:0.3_0": "The Elder Nun Sañjaya",
    "ai-tha-ap526:1.1_0": "“You’re not bound with a harem,",
    "ai-tha-ap526:1.2_0": "I’ll dwell in the royal compound.",
    "ai-tha-ap526:1.3_0": "I sat down on the four legs.",
    "ai-tha-ap526:1.4_0": "I’m the one who broke the form of a kasiṇa,",
    "ai-tha-ap526:2.1_0": "I saw the Buddha",
    "ai-tha-ap526:2.2_0": "or mangoes?",
    "ai-tha-ap526:2.3_0": "resolutely, resolute,",
    "ai-tha-ap526:2.4_0": "delighting in absorption,",
    "ai-tha-ap526:3.1_0": "whose every desire is granted:",
    "ai-tha-ap526:3.2_0": "like a flood-crosser.",
    "ai-tha-ap526:3.3_0": "Having seen clearly with confident heart,",
    "ai-tha-ap526:3.4_0": "Which cries",
    "ai-tha-ap526:4.1_0": "A single eon irrevers:",
    "ai-tha-ap526:4.2_0": "Giving a gift (of giving) gifts,",
    "ai-tha-ap526:4.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap526:4.4_0": "this ointment is fruitful.",
    "ai-tha-ap526:5.1_0": "corrupted by defilements,",
    "ai-tha-ap526:5.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap526:5.3_0": "and live without defilements.",
    "ai-tha-ap526:6.1_0": "It was so welcome for me",
    "ai-tha-ap526:6.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap526:6.3_0": "and fulfilled the Buddha’s instructions.",
    "ai-tha-ap526:7.1_0": "the four analytical knowledges,",
    "ai-tha-ap526:7.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap526:7.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap526:8.1_0": "That is how these verses were recited by the senior venerable Visākhā.",
    "ai-tha-ap526:9.1_0": "The fourth Elder Nun Sañjaya"
}