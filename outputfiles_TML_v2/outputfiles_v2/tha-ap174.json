{
    "ai-tha-ap174:0.1_0": "Therāpadāna",
    "ai-tha-ap174:0.2_0": "Kumudavagga",
    "ai-tha-ap174:0.3_0": "2 Nisseṇidāyakattheraapadāna",
    "ai-tha-ap174:1.1_0": "“Surely the Fortunate One,",
    "ai-tha-ap174:1.2_0": "poised one in the world.",
    "ai-tha-ap174:1.3_0": "I climbed the tower of the stilt longhouse,",
    "ai-tha-ap174:1.4_0": "I’ve made a resplendent.",
    "ai-tha-ap174:2.1_0": "with faithful mind,",
    "ai-tha-ap174:2.2_0": "attaining the end of course,",
    "ai-tha-ap174:2.3_0": "I bear their final body,",
    "ai-tha-ap174:2.4_0": "in the teaching of the Buddha,",
    "ai-tha-ap174:3.1_0": "Thirty eons",
    "ai-tha-ap174:3.2_0": "for I am three thousand ...",
    "ai-tha-ap174:3.3_0": "A great king:",
    "ai-tha-ap174:3.4_0": "The Many-Carried wheels",
    "ai-tha-ap174:4.1_0": "the four analytical knowledges,",
    "ai-tha-ap174:4.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap174:4.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap174:5.1_0": "That is how these verses were recited by the senior venerable Nigrodha.",
    "ai-tha-ap174:6.1_0": "The second rainy-season residence of the Elder Nuns, (saying):"
}