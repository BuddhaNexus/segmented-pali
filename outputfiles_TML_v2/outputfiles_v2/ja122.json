{
    "ai-ja122:0.1_0": "Jātaka",
    "ai-ja122:0.2_0": "Ekakanipāta",
    "ai-ja122:0.3_0": "Kusanāḷivagga",
    "ai-ja122:0.4_0": "2. Dummedhajātaka",
    "ai-ja122:1.1_0": "“When you’ve gained the fool,",
    "ai-ja122:1.2_0": "they do not know what is good,",
    "ai-ja122:1.3_0": "you act for the good of both",
    "ai-ja122:1.4_0": "for the noble ones.”",
    "ai-ja122:2.1_0": "the second boy was born."
}