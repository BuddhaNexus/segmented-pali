{
    "ai-tha-ap223:0.1_0": "Therāpadāna",
    "ai-tha-ap223:0.2_0": "Ālambaṇadāyakavagga",
    "ai-tha-ap223:0.3_0": "1 Ālambaṇadāyakattheraapadāna",
    "ai-tha-ap223:1.1_0": "“I saw the Fortunate One’s body,",
    "ai-tha-ap223:1.2_0": "poised one in the world.",
    "ai-tha-ap223:1.3_0": "I’ve given a gift to my mangoes.",
    "ai-tha-ap223:1.4_0": "the poised one, the perfected one;",
    "ai-tha-ap223:2.1_0": "I practice the way",
    "ai-tha-ap223:2.2_0": "abundant wealth, immeasurable,",
    "ai-tha-ap223:2.3_0": "wandering among the seers.",
    "ai-tha-ap223:2.4_0": "practicing absorption;",
    "ai-tha-ap223:3.1_0": "corrupted by defilements,",
    "ai-tha-ap223:3.2_0": "rebirth into all states of existence is eradicated,",
    "ai-tha-ap223:3.3_0": "I’ve attained the three knowledges",
    "ai-tha-ap223:3.4_0": "and fulfilled the Buddha’s instructions.",
    "ai-tha-ap223:4.1_0": "Except in the past.",
    "ai-tha-ap223:4.2_0": "There were three aristocrats,",
    "ai-tha-ap223:4.3_0": "One day has an outcaste.",
    "ai-tha-ap223:4.4_0": "The Many-Carried wheels",
    "ai-tha-ap223:5.1_0": "the four analytical knowledges,",
    "ai-tha-ap223:5.2_0": "having excellence and liberation,",
    "ai-tha-ap223:5.3_0": "I’ve realized the six kinds of direct knowledge,",
    "ai-tha-ap223:5.4_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap223:6.1_0": "That is how these verses were recited by the senior venerable Kambosambī.",
    "ai-tha-ap223:7.1_0": "First the first courtesan Ambapālī"
}