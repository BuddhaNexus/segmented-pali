{
    "ai-thig1.13:0.1_0": "Therīgāthā",
    "ai-thig1.13:0.2_0": "Ekakanipāta",
    "ai-thig1.13:0.3_0": "13. Visākhātherīgāthā",
    "ai-thig1.13:1.1_0": "“Answer the Buddha’s bidding,",
    "ai-thig1.13:1.2_0": "one that does not plague you.",
    "ai-thig1.13:1.3_0": "Having quickly washed your feet,",
    "ai-thig1.13:1.4_0": "sit in a discreet place to meditate.",
    "ai-thig1.13:2.1_0": "Visākhā,"
}