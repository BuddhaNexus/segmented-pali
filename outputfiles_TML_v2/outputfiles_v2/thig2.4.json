{
    "ai-thig2.4:0.1_0": "Therīgāthā",
    "ai-thig2.4:0.2_0": "Dukanipāta",
    "ai-thig2.4:0.3_0": "4 Aḍḍhakāsitherīgāthā",
    "ai-thig2.4:1.1_0": "The price for my services",
    "ai-thig2.4:1.2_0": "I’ve heard my cry,",
    "ai-thig2.4:1.3_0": "By setting that price,",
    "ai-thig2.4:1.4_0": "the townsfolk made me priceless.",
    "ai-thig2.4:2.1_0": "Then, growing disillusioned with my form,",
    "ai-thig2.4:2.2_0": "I became dispassionate.",
    "ai-thig2.4:2.3_0": "Don’t journey on and on,",
    "ai-thig2.4:2.4_0": "transmigrating through rebirths!",
    "ai-thig2.4:2.5_0": "I've realized the three knowledges,",
    "ai-thig2.4:2.6_0": "and fulfilled the Buddha’s instructions.",
    "ai-thig2.4:3.1_0": "... a senior nun ..."
}