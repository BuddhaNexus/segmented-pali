{
    "ai-thag1.96:0.1_0": "Theragāthā",
    "ai-thag1.96:0.2_0": "Ekakanipāta",
    "ai-thag1.96:0.3_0": "Dasamavagga",
    "ai-thag1.96:0.4_0": "1.96. Khaṇḍasumanattheragāthā",
    "ai-thag1.96:1.1_0": "I offered a single flower",
    "ai-thag1.96:1.2_0": "and then amused myself in heavens",
    "ai-thag1.96:1.3_0": "for 800 million years;",
    "ai-thag1.96:1.4_0": "with what’s left over I’ve become quenched.",
    "ai-thag1.96:1.5_0": "Khaṇḍasumana,"
}