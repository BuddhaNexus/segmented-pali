{
    "ai-tha-ap369:0.1_0": "Therāpadāna",
    "ai-tha-ap369:0.2_0": "Mandāravapupphiyavagga",
    "ai-tha-ap369:0.3_0": "The Elder Uppalavaṇṇā",
    "ai-tha-ap369:1.1_0": "“Awakened! He is called “Buddha”.",
    "ai-tha-ap369:1.2_0": "Then at the foot of the Ganges,",
    "ai-tha-ap369:1.3_0": "After grabbing a hunter",
    "ai-tha-ap369:1.4_0": "revered by gods and humans.",
    "ai-tha-ap369:2.1_0": "It’s been thirty one eons",
    "ai-tha-ap369:2.2_0": "As a bird was worshipped by the flowers,",
    "ai-tha-ap369:2.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap369:2.4_0": "this fruit is to know,",
    "ai-tha-ap369:3.1_0": "the four analytical knowledges,",
    "ai-tha-ap369:3.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap369:3.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap369:4.1_0": "That is how these verses were recited by the senior venerable Universal Monarch.",
    "ai-tha-ap369:5.1_0": "the seventh Cowherd thus (of flowers),"
}