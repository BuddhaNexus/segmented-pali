{
    "ai-tha-ap237:0.1_0": "Therāpadāna",
    "ai-tha-ap237:0.2_0": "Udakāsanavagga",
    "ai-tha-ap237:0.3_0": "5 Vedikārakattheraapadāna",
    "ai-tha-ap237:1.1_0": "“Look at the Fortunate One’s Tapassī,",
    "ai-tha-ap237:1.2_0": "the Buddha-to-be,",
    "ai-tha-ap237:1.3_0": "confident in heart,",
    "ai-tha-ap237:1.4_0": "I feel undergoing suffering,",
    "ai-tha-ap237:2.1_0": "A single eon irreversible eons",
    "ai-tha-ap237:2.2_0": "I feel undergoing suffering,",
    "ai-tha-ap237:2.3_0": "I’m not aware of any living being",
    "ai-tha-ap237:2.4_0": "this is the fruit.",
    "ai-tha-ap237:3.1_0": "One day, after going to the charnel ground,",
    "ai-tha-ap237:3.2_0": "I pursued the sun.",
    "ai-tha-ap237:3.3_0": "endowed with the seven treasures,",
    "ai-tha-ap237:3.4_0": "The Mahallika conversions",
    "ai-tha-ap237:4.1_0": "the four analytical knowledges,",
    "ai-tha-ap237:4.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap237:4.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap237:5.1_0": "That is how these verses were recited by the senior venerable Pañcasikha.",
    "ai-tha-ap237:6.1_0": "for the fifth time (where the Elder Understanding):"
}