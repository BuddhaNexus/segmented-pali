{
    "ai-thi-ap13:0.1_0": "Therīapadāna",
    "ai-thi-ap13:0.2_0": "Ekūposathikavagga",
    "ai-thi-ap13:0.3_0": "3. Modakadāyikātherīapadāna",
    "ai-thi-ap13:1.1_0": "“You’re not bound with a harem,",
    "ai-thi-ap13:1.2_0": "I’ll be a jar,",
    "ai-thi-ap13:1.3_0": "take my home and go to my family,",
    "ai-thi-ap13:1.4_0": "I plunged into the water;",
    "ai-thi-ap13:2.1_0": "Having seen (that), ascetic, you must go forth!",
    "ai-thi-ap13:2.2_0": "peaceful, serene,",
    "ai-thi-ap13:2.3_0": "being confident in confident heart,",
    "ai-thi-ap13:2.4_0": "for the three generals.",
    "ai-thi-ap13:3.1_0": "Through that deed",
    "ai-thi-ap13:3.2_0": "Though there are three faces,",
    "ai-thi-ap13:3.3_0": "One living alone:",
    "ai-thi-ap13:3.4_0": "“I won’t go anywhere,",
    "ai-thi-ap13:4.1_0": "Flame, they make a wish,",
    "ai-thi-ap13:4.2_0": "I am the one with all continuations,",
    "ai-thi-ap13:4.3_0": "given the three sweetnesss,",
    "ai-thi-ap13:4.4_0": "I’ve attained the unshakable,",
    "ai-thi-ap13:5.1_0": "corrupted by defilements,",
    "ai-thi-ap13:5.2_0": "... “I’ll sleep ...”",
    "ai-thi-ap13:5.3_0": "and live without defilements.",
    "ai-thi-ap13:6.1_0": "It was so welcome for me",
    "ai-thi-ap13:6.2_0": "... “I’ll sleep ...”",
    "ai-thi-ap13:6.3_0": "and fulfilled the Buddha’s instructions.",
    "ai-thi-ap13:7.1_0": "the four analytical knowledges,",
    "ai-thi-ap13:7.2_0": "... “I’ll sleep ...”",
    "ai-thi-ap13:7.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-thi-ap13:8.1_0": "That is how these verses were recited by the nun Phagguna.",
    "ai-thi-ap13:9.1_0": "Third was the third Elder Nun Pañcālaka"
}