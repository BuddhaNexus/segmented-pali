{
    "ai-tha-ap238:0.1_0": "Therāpadāna",
    "ai-tha-ap238:0.2_0": "Udakāsanavagga",
    "ai-tha-ap238:0.3_0": "6 Vaṇṇakārattheraapadāna",
    "ai-tha-ap238:1.1_0": "“You’re not like a fool,",
    "ai-tha-ap238:1.2_0": "I am observed by you.",
    "ai-tha-ap238:1.3_0": "shrines with the twellers’ shrines",
    "ai-tha-ap238:1.4_0": "different colors,",
    "ai-tha-ap238:2.1_0": "It’s been thirty one eons",
    "ai-tha-ap238:2.2_0": "the dust-pickable,",
    "ai-tha-ap238:2.3_0": "I’m not aware of any living being",
    "ai-tha-ap238:2.4_0": "as gifts of the fruit.",
    "ai-tha-ap238:3.1_0": "Twenty by twenty fats on them.",
    "ai-tha-ap238:3.2_0": "one whose colour is lethaved.",
    "ai-tha-ap238:3.3_0": "endowed with the seven treasures,",
    "ai-tha-ap238:3.4_0": "The Mahallika conversions",
    "ai-tha-ap238:4.1_0": "the four analytical knowledges,",
    "ai-tha-ap238:4.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap238:4.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap238:5.1_0": "That is how these verses were recited by the senior venerable S praised.",
    "ai-tha-ap238:6.1_0": "In the sixth enquiry, (called) a sixthous ceremonies,"
}