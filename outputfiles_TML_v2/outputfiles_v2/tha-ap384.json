{
    "ai-tha-ap384:0.1_0": "Therāpadāna",
    "ai-tha-ap384:0.2_0": "Avaṭaphalavagga",
    "ai-tha-ap384:0.3_0": "2. Labujadāyakattheraapadāna",
    "ai-tha-ap384:1.1_0": "“You’re not bound with a harem,",
    "ai-tha-ap384:1.2_0": "I was a monastery worker.",
    "ai-tha-ap384:1.3_0": "I saw the stainless Buddha",
    "ai-tha-ap384:1.4_0": "walking on the road,",
    "ai-tha-ap384:2.1_0": "obtaining what can be gained,",
    "ai-tha-ap384:2.2_0": "before the best of Buddhas.",
    "ai-tha-ap384:2.3_0": "steady,",
    "ai-tha-ap384:2.4_0": "great hero, after accepting a great sacrifice,",
    "ai-tha-ap384:3.1_0": "and roaming the forehead.",
    "ai-tha-ap384:3.2_0": "The happiness of this life,",
    "ai-tha-ap384:3.3_0": "give him fruit to the Buddha,",
    "ai-tha-ap384:3.4_0": "with clear and confident heart,",
    "ai-tha-ap384:4.1_0": "obtaining happiness,",
    "ai-tha-ap384:4.2_0": "I understood the best happiness.",
    "ai-tha-ap384:4.3_0": "just by those treasures.",
    "ai-tha-ap384:4.4_0": "by the Buddha’s rebirth is this—",
    "ai-tha-ap384:5.1_0": "A single eon irrevers:",
    "ai-tha-ap384:5.2_0": "I used to be a fruit.",
    "ai-tha-ap384:5.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap384:5.4_0": "as fruit is the fruit.",
    "ai-tha-ap384:6.1_0": "corrupted by defilements,",
    "ai-tha-ap384:6.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap384:6.3_0": "and live without defilements.",
    "ai-tha-ap384:7.1_0": "It was so welcome for me",
    "ai-tha-ap384:7.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap384:7.3_0": "and fulfilled the Buddha’s instructions.",
    "ai-tha-ap384:8.1_0": "the four analytical knowledges,",
    "ai-tha-ap384:8.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap384:8.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap384:9.1_0": "That is how these verses were recited by the senior venerable Jotipāla.",
    "ai-tha-ap384:10.1_0": "Seeing the second (said): Rojaya, (Tree)"
}