{
    "ai-tha-ap457:0.1_0": "Therāpadāna",
    "ai-tha-ap457:0.2_0": "Jagatidāyakavagga",
    "ai-tha-ap457:0.3_0": "Queen Saviṭṭhaka",
    "ai-tha-ap457:1.1_0": "“When will you sage",
    "ai-tha-ap457:1.2_0": "the accomplished brahmin!",
    "ai-tha-ap457:1.3_0": "ageing in all my dwellings,",
    "ai-tha-ap457:1.4_0": "Queen Māyā",
    "ai-tha-ap457:2.1_0": "in this life they beside.",
    "ai-tha-ap457:2.2_0": "Giving a gift (of giving) gifts,",
    "ai-tha-ap457:2.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap457:2.4_0": "this fruit is pulled out.",
    "ai-tha-ap457:3.1_0": "corrupted by defilements,",
    "ai-tha-ap457:3.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap457:3.3_0": "and live without defilements.",
    "ai-tha-ap457:4.1_0": "It was so welcome for me",
    "ai-tha-ap457:4.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap457:4.3_0": "and fulfilled the Buddha’s instructions.",
    "ai-tha-ap457:5.1_0": "the four analytical knowledges,",
    "ai-tha-ap457:5.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap457:5.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap457:6.1_0": "That is how these verses were recited by the senior venerable Abhibhū.",
    "ai-tha-ap457:7.1_0": "The fifth Elder Upavāṇa"
}