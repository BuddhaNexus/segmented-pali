{
    "ai-tha-ap506:0.1_0": "Therāpadāna",
    "ai-tha-ap506:0.2_0": "Kaṇikāravagga",
    "ai-tha-ap506:0.3_0": "4. Avaṭaphaliyattheraapadāna",
    "ai-tha-ap506:1.1_0": "“Over a thousand people Fortunate One,",
    "ai-tha-ap506:1.2_0": "if they sleep like a sage should beamed;",
    "ai-tha-ap506:1.3_0": "Having risen above seclusion,",
    "ai-tha-ap506:1.4_0": "I went out of my resort.",
    "ai-tha-ap506:2.1_0": "I’ve seen fruits in the fruit,",
    "ai-tha-ap506:2.2_0": "the fear of guilt,",
    "ai-tha-ap506:2.3_0": "confident in heart,",
    "ai-tha-ap506:2.4_0": "took a fruit of what is not good,",
    "ai-tha-ap506:3.1_0": "if he makes his four eons;",
    "ai-tha-ap506:3.2_0": "the fruitless (of rebirth).",
    "ai-tha-ap506:3.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap506:3.4_0": "as fruit is the fruit.",
    "ai-tha-ap506:4.1_0": "corrupted by defilements,",
    "ai-tha-ap506:4.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap506:4.3_0": "and live without defilements.",
    "ai-tha-ap506:5.1_0": "It was so welcome for me",
    "ai-tha-ap506:5.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap506:5.3_0": "and fulfilled the Buddha’s instructions.",
    "ai-tha-ap506:6.1_0": "the four analytical knowledges,",
    "ai-tha-ap506:6.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap506:6.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap506:7.1_0": "That is how these verses were recited by the senior venerable Piṭaka.",
    "ai-tha-ap506:8.1_0": "took the fourth Elder Piṭaka"
}