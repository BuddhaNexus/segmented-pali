{
    "ai-ja148:0.1_0": "Jātaka",
    "ai-ja148:0.2_0": "Ekakanipāta",
    "ai-ja148:0.3_0": "Kakaṇṭakavagga",
    "ai-ja148:0.4_0": "8. Siṅgālajātaka",
    "ai-ja148:1.1_0": "I didn’t return again,",
    "ai-ja148:1.2_0": "nor will the wise",
    "ai-ja148:1.3_0": "I will tell you an elephant,",
    "ai-ja148:1.4_0": "for it is because of fear.”",
    "ai-ja148:2.1_0": "like the eighth cowherd’s."
}