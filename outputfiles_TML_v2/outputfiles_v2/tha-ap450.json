{
    "ai-tha-ap450:0.1_0": "Therāpadāna",
    "ai-tha-ap450:0.2_0": "Vibhītakavagga",
    "ai-tha-ap450:0.3_0": "8. Pādapīṭhiyattheraapadāna",
    "ai-tha-ap450:1.1_0": "“Awakened by the Perfectly Awakened One,",
    "ai-tha-ap450:1.2_0": "the sage, like a curtain.",
    "ai-tha-ap450:1.3_0": "After making many offerings, many hundreds,",
    "ai-tha-ap450:1.4_0": "the Greatly Dispensation,",
    "ai-tha-ap450:2.1_0": "I’m sitting down, seeking for myself,",
    "ai-tha-ap450:2.2_0": "of good conduct, immaculate:",
    "ai-tha-ap450:2.3_0": "confident in heart,",
    "ai-tha-ap450:2.4_0": "he presented his feet.",
    "ai-tha-ap450:3.1_0": "having done many good deeds,",
    "ai-tha-ap450:3.2_0": "whose happy outcome is happiness,",
    "ai-tha-ap450:3.3_0": "and he is detached from making merit.",
    "ai-tha-ap450:3.4_0": "I went to the realm of the Thirty-Three.",
    "ai-tha-ap450:4.1_0": "my jungle thicket was like that.",
    "ai-tha-ap450:4.2_0": "the legal procedure of merit:",
    "ai-tha-ap450:4.3_0": "if he removes the steps;",
    "ai-tha-ap450:4.4_0": "My ears are full of light,",
    "ai-tha-ap450:5.1_0": "You’re fortunate, so very fortunate,",
    "ai-tha-ap450:5.2_0": "Endowed, they get familiar with him:",
    "ai-tha-ap450:5.3_0": "After taking possession of the Emancipation,",
    "ai-tha-ap450:5.4_0": "so abundant happiness,",
    "ai-tha-ap450:6.1_0": "I’ve done bad deeds,",
    "ai-tha-ap450:6.2_0": "with the wind dispelled,",
    "ai-tha-ap450:6.3_0": "I should make a foot-stool;",
    "ai-tha-ap450:6.4_0": "I embarrassed,",
    "ai-tha-ap450:7.1_0": "whatever direction I leave",
    "ai-tha-ap450:7.2_0": "Why?",
    "ai-tha-ap450:7.3_0": "I stepped on a letter.",
    "ai-tha-ap450:7.4_0": "the fruit of good deeds.",
    "ai-tha-ap450:8.1_0": "The thirty-two thousand chiefs",
    "ai-tha-ap450:8.2_0": "I wandered about the (good) actions.",
    "ai-tha-ap450:8.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap450:8.4_0": "this is the bedroom, this is the fruit.",
    "ai-tha-ap450:9.1_0": "corrupted by defilements,",
    "ai-tha-ap450:9.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap450:9.3_0": "and live without defilements.",
    "ai-tha-ap450:10.1_0": "It was so welcome for me",
    "ai-tha-ap450:10.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap450:10.3_0": "and fulfilled the Buddha’s instructions.",
    "ai-tha-ap450:11.1_0": "the four analytical knowledges,",
    "ai-tha-ap450:11.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap450:11.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap450:12.1_0": "That is how these verses were recited by the senior venerable Nunāṭa.",
    "ai-tha-ap450:13.1_0": "eighth Elder Nuns"
}