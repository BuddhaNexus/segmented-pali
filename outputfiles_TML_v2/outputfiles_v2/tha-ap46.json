{
    "ai-tha-ap46:0.1_0": "Therāpadāna",
    "ai-tha-ap46:0.2_0": "Upālivagga",
    "ai-tha-ap46:0.3_0": "4. Sanniṭṭhāpakattheraapadāna",
    "ai-tha-ap46:1.1_0": "“Having built a little hut in the wilderness,",
    "ai-tha-ap46:1.2_0": "I stay in a mountain cleft.",
    "ai-tha-ap46:1.3_0": "Contentment, contentment,",
    "ai-tha-ap46:1.4_0": "With the giving up of fame",
    "ai-tha-ap46:2.1_0": "Padumuttara, the world.",
    "ai-tha-ap46:2.2_0": "receiver of sacrifices,",
    "ai-tha-ap46:2.3_0": "You can dwell on the road",
    "ai-tha-ap46:2.4_0": "I’ve come into my presence,",
    "ai-tha-ap46:3.1_0": "they’re tormenting a great nāga.",
    "ai-tha-ap46:3.2_0": "the damage in conqueror,",
    "ai-tha-ap46:3.3_0": "spreading over the grass,",
    "ai-tha-ap46:3.4_0": "I practise a sword,",
    "ai-tha-ap46:4.1_0": "confident in heart,",
    "ai-tha-ap46:4.2_0": "I drink the water, and the water for washing—",
    "ai-tha-ap46:4.3_0": "When I was straight,",
    "ai-tha-ap46:4.4_0": "with clear and confident heart,",
    "ai-tha-ap46:5.1_0": "it’s a hundred thousand eons",
    "ai-tha-ap46:5.2_0": "Giving a gift (of giving) gifts,",
    "ai-tha-ap46:5.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap46:5.4_0": "this is the fruit for the living.",
    "ai-tha-ap46:6.1_0": "I’m satisfied with one wrapped in the sea",
    "ai-tha-ap46:6.2_0": "I gave him a bear,",
    "ai-tha-ap46:6.3_0": "endowed with the seven treasures,",
    "ai-tha-ap46:6.4_0": "The Mahallika conversions",
    "ai-tha-ap46:7.1_0": "the four analytical knowledges,",
    "ai-tha-ap46:7.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap46:7.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap46:8.1_0": "That is how these verses were recited by the senior venerable Senior.",
    "ai-tha-ap46:9.1_0": "The fourth Elder Aṅgaka"
}