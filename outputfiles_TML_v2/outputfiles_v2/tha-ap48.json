{
    "ai-tha-ap48:0.1_0": "Therāpadāna",
    "ai-tha-ap48:0.2_0": "Upālivagga",
    "ai-tha-ap48:0.3_0": "The Elder Nun Bhaddiya",
    "ai-tha-ap48:1.1_0": "“I’m extinguished in the world,",
    "ai-tha-ap48:1.2_0": "Persons one who understands",
    "ai-tha-ap48:1.3_0": "the top of the dreams,",
    "ai-tha-ap48:1.4_0": "I announced my doubts,",
    "ai-tha-ap48:2.1_0": "and even a fraction",
    "ai-tha-ap48:2.2_0": "I’m not stuck in the air.",
    "ai-tha-ap48:2.3_0": "and putting on a sunshade,",
    "ai-tha-ap48:2.4_0": "made a funeral pyre.",
    "ai-tha-ap48:3.1_0": "A single eon irrevers:",
    "ai-tha-ap48:3.2_0": "it was all over the flower.",
    "ai-tha-ap48:3.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap48:3.4_0": "this fruit is to know,",
    "ai-tha-ap48:4.1_0": "Seven eons have been expanded.",
    "ai-tha-ap48:4.2_0": "I’m mad, excited,",
    "ai-tha-ap48:4.3_0": "with a victory:",
    "ai-tha-ap48:4.4_0": "The Mahallika conversions",
    "ai-tha-ap48:5.1_0": "the four analytical knowledges,",
    "ai-tha-ap48:5.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap48:5.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap48:6.1_0": "That is how these verses were recited by the senior venerable Padumavatī.",
    "ai-tha-ap48:7.1_0": "he gave the sixth and the Elder Nun Padumavatī"
}