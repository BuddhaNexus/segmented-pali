{
    "ai-tha-ap203:0.1_0": "Therāpadāna",
    "ai-tha-ap203:0.2_0": "Kaṇikārapupphiyavagga",
    "ai-tha-ap203:0.3_0": "The Elder Kaṇṭaka flowers",
    "ai-tha-ap203:1.1_0": "“Having seen the flower’s neck,",
    "ai-tha-ap203:1.2_0": "I encountered,",
    "ai-tha-ap203:1.3_0": "I cured him.",
    "ai-tha-ap203:1.4_0": "Having crossed the flood’s poised?",
    "ai-tha-ap203:2.1_0": "On the two occasions when the other goes forth",
    "ai-tha-ap203:2.2_0": "it was all over the flower.",
    "ai-tha-ap203:2.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap203:2.4_0": "this fruit is to know,",
    "ai-tha-ap203:3.1_0": "falling into five eons.",
    "ai-tha-ap203:3.2_0": "free of lust, stainless:",
    "ai-tha-ap203:3.3_0": "endowed with the seven treasures,",
    "ai-tha-ap203:3.4_0": "The Mahallika conversions",
    "ai-tha-ap203:4.1_0": "the four analytical knowledges,",
    "ai-tha-ap203:4.2_0": "having excellence and liberation,",
    "ai-tha-ap203:4.3_0": "I’ve realized the six kinds of direct knowledge,",
    "ai-tha-ap203:4.4_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap203:5.1_0": "That is how these verses were recited by the senior venerable Kaṇṇiya.",
    "ai-tha-ap203:6.1_0": "First the first Coral Tree"
}