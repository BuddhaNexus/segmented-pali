{
    "ai-ja63:0.1_0": "Jātaka",
    "ai-ja63:0.2_0": "Ekakanipāta",
    "ai-ja63:0.3_0": "Itthivagga",
    "ai-ja63:0.4_0": "3. Takkapaṇḍitajātaka",
    "ai-ja63:1.1_0": "Free of anger and kindness,",
    "ai-ja63:1.2_0": "who cause division against friends.",
    "ai-ja63:1.3_0": "a mendicant should live the spiritual life,",
    "ai-ja63:1.4_0": "and your happiness never fails.",
    "ai-ja63:2.1_0": "and third from Takkasilā."
}