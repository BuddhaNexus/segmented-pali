{
    "ai-tha-ap120:0.1_0": "Therāpadāna",
    "ai-tha-ap120:0.2_0": "Mahāparivāravagga",
    "ai-tha-ap120:0.3_0": "8. Maggasaññakattheraapadāna",
    "ai-tha-ap120:1.1_0": "“Padumuttara, the Buddha Padumuttara,",
    "ai-tha-ap120:1.2_0": "if the disciples live apartly.",
    "ai-tha-ap120:1.3_0": "There are eighteen in the forest,",
    "ai-tha-ap120:1.4_0": "Intoxicated with the belief of blind men.",
    "ai-tha-ap120:2.1_0": "After recollecting the Buddha,",
    "ai-tha-ap120:2.2_0": "Padumuttara,",
    "ai-tha-ap120:2.3_0": "you’ll have a little child.",
    "ai-tha-ap120:2.4_0": "Dwelling in the forest,",
    "ai-tha-ap120:3.1_0": "Having given up continued existence,",
    "ai-tha-ap120:3.2_0": "The monks went to a monk.",
    "ai-tha-ap120:3.3_0": "explaining them to them,",
    "ai-tha-ap120:3.4_0": "I’ll eat unguarded food,",
    "ai-tha-ap120:4.1_0": "Through that deed and its result,",
    "ai-tha-ap120:4.2_0": "nor in the world’s end.",
    "ai-tha-ap120:4.3_0": "At seven years old",
    "ai-tha-ap120:4.4_0": "I attained Worthiness",
    "ai-tha-ap120:5.1_0": "the name Nādikas said:",
    "ai-tha-ap120:5.2_0": "The Thirty-Two Monarchs",
    "ai-tha-ap120:5.3_0": "Endowed with the seven gems,",
    "ai-tha-ap120:5.4_0": "the five hundred lives (of them).",
    "ai-tha-ap120:6.1_0": "the four analytical knowledges,",
    "ai-tha-ap120:6.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap120:6.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap120:7.1_0": "That is how these verses were recited by the senior venerable Makhādeva.",
    "ai-tha-ap120:8.1_0": "eighth Elder Nuns"
}