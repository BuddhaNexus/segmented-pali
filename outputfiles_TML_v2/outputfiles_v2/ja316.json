    "ai-ja316:0.4_0": "6. Sasapaṇḍitajātaka",
    "ai-ja316:1.1_0": "The person with a morsel",
    "ai-ja316:1.2_0": "like a stump,",
    "ai-ja316:1.3_0": "I have this brahmin;",
    "ai-ja316:1.4_0": "in the jungle thicket.",
    "ai-ja316:2.1_0": "“See the field,",
    "ai-ja316:2.2_0": "night and day.",
    "ai-ja316:2.3_0": "flesh and cattle,",
    "ai-ja316:2.4_0": "Their bodies shake,",
    "ai-ja316:2.5_0": "I have this brahmin;",
    "ai-ja316:2.6_0": "in the jungle thicket.",
    "ai-ja316:3.1_0": "“Living in a cool breeze,",
    "ai-ja316:3.2_0": "like a cool lion,",
    "ai-ja316:3.3_0": "I have this brahmin;",
    "ai-ja316:3.4_0": "in the jungle thicket.",
    "ai-ja316:4.1_0": "“There is no droping means",
