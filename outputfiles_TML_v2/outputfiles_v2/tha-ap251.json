{
    "ai-tha-ap251:0.1_0": "Therāpadāna",
    "ai-tha-ap251:0.2_0": "Tuvaradāyakavagga",
    "ai-tha-ap251:0.3_0": "9 Bimbijāliyattheraapadāna",
    "ai-tha-ap251:1.1_0": "“The victor named Uttara.",
    "ai-tha-ap251:1.2_0": "or like the great person,",
    "ai-tha-ap251:1.3_0": "Truth it is the fourth:",
    "ai-tha-ap251:1.4_0": "they proclaim the deathless.",
    "ai-tha-ap251:2.1_0": "My net was full of flowers,",
    "ai-tha-ap251:2.2_0": "Having performed (them) from the earth,",
    "ai-tha-ap251:2.3_0": "I rejoice in the Buddha’s instruction.",
    "ai-tha-ap251:2.4_0": "the poised one, the perfected one;",
    "ai-tha-ap251:3.1_0": "In eighth eons",
    "ai-tha-ap251:3.2_0": "Fourth: largeings,",
    "ai-tha-ap251:3.3_0": "Endowed with the seven gems,",
    "ai-tha-ap251:3.4_0": "The Many-Carried wheels",
    "ai-tha-ap251:4.1_0": "the four analytical knowledges,",
    "ai-tha-ap251:4.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap251:4.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap251:5.1_0": "That is how these verses were recited by the senior venerable Bāliya.",
    "ai-tha-ap251:6.1_0": "Uppalavaṇṇā, having nine Elders built,"
}