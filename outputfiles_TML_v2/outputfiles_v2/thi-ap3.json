{
    "ai-thi-ap3:0.1_0": "Therīapadāna",
    "ai-thi-ap3:0.2_0": "Sumedhāvagga",
    "ai-thi-ap3:0.3_0": "3. Maṇḍapadāyikātherīapadāna",
    "ai-thi-ap3:1.1_0": "“Koṇāgamana, the Buddha,",
    "ai-thi-ap3:1.2_0": "I’ve built a mosquito,",
    "ai-thi-ap3:1.3_0": "I gave a robe to Tissa.",
    "ai-thi-ap3:1.4_0": "the Buddha, Kinsman of the world.",
    "ai-thi-ap3:2.1_0": "Whatever country I went to,",
    "ai-thi-ap3:2.2_0": "in town and capital cities.",
    "ai-thi-ap3:2.3_0": "I have constantly worshipped all,",
    "ai-thi-ap3:2.4_0": "the fruit of good deeds.",
    "ai-thi-ap3:3.1_0": "corrupted by defilements,",
    "ai-thi-ap3:3.2_0": "... “I’ll sleep ...”",
    "ai-thi-ap3:3.3_0": "and live without defilements.",
    "ai-thi-ap3:4.1_0": "It was so welcome for me",
    "ai-thi-ap3:4.2_0": "... “I’ll sleep ...”",
    "ai-thi-ap3:4.3_0": "and fulfilled the Buddha’s instructions.",
    "ai-thi-ap3:5.1_0": "the four analytical knowledges,",
    "ai-thi-ap3:5.2_0": "... “I’ll sleep ...”",
    "ai-thi-ap3:5.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-thi-ap3:6.1_0": "That is how these verses were recited by the nun Macchikāsaṇḍa.",
    "ai-thi-ap3:7.1_0": "third Elder Macchikāsaṇḍa"
}