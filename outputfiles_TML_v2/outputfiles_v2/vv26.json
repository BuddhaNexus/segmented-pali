{
    "ai-vv26:0.1_0": "Vimānavatthu",
    "ai-vv26:0.2_0": "Itthivimāna",
    "ai-vv26:0.3_0": "Cittalatāvagga",
    "ai-vv26:0.4_0": "9. Suniddāvimānavatthu",
    "ai-vv26:1.1_0": "Avoiding beauty",
    "ai-vv26:1.2_0": "... “I’ll sleep ...”",
    "ai-vv26:1.3_0": "like a daughter-in-law of a low-worm,",
    "ai-vv26:2.1_0": "What is your beautiful complexion?",
    "ai-vv26:2.2_0": "... “I’ll sleep ...”",
    "ai-vv26:2.3_0": "so beautiful and ugly.”",
    "ai-vv26:3.1_0": "This is their delight,",
    "ai-vv26:3.2_0": "... “I’ll sleep ...”",
    "ai-vv26:3.3_0": "It is the fruit of that deed,",
    "ai-vv26:4.1_0": "“She is enlightenment in her.",
    "ai-vv26:4.2_0": "a female lay follower",
    "ai-vv26:4.3_0": "faithful, accomplished in ethics,",
    "ai-vv26:4.4_0": "restraint, and constraint,",
    "ai-vv26:5.1_0": "(…)",
    "ai-vv26:6.1_0": "Pañcasikha had developed the five wives.",
    "ai-vv26:6.2_0": "who see the noble truths.",
    "ai-vv26:6.3_0": "the eye from the laywomen.",
    "ai-vv26:6.4_0": "of the renowned Gotama",
    "ai-vv26:7.1_0": "That’s why I praise",
    "ai-vv26:7.2_0": "... “I’ll sleep ...”",
    "ai-vv26:7.3_0": "and my complexion was like me.”",
    "ai-vv26:8.1_0": "the ninth section on the ninth,"
}