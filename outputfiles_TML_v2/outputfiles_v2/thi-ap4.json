{
    "ai-thi-ap4:0.1_0": "Therīapadāna",
    "ai-thi-ap4:0.2_0": "Sumedhāvagga",
    "ai-thi-ap4:0.3_0": "4 Saṅkamanatthātherīapadāna",
    "ai-thi-ap4:1.1_0": "“Vipassī, the Fortunate One’s Dhamma,",
    "ai-thi-ap4:1.2_0": "poised one in the world.",
    "ai-thi-ap4:1.3_0": "if you’re prepared a chariot;",
    "ai-thi-ap4:1.4_0": "the end of all creatures.",
    "ai-thi-ap4:2.1_0": "I left my house",
    "ai-thi-ap4:2.2_0": "Stepping down, lie down,",
    "ai-thi-ap4:2.3_0": "the lord of the world,",
    "ai-thi-ap4:2.4_0": "Give me the chance:",
    "ai-thi-ap4:3.1_0": "having neglected heads,",
    "ai-thi-ap4:3.2_0": "is far from the world—",
    "ai-thi-ap4:3.3_0": "with faithful mind,",
    "ai-thi-ap4:3.4_0": "Tusitasena, I’ve brought the lady",
    "ai-thi-ap4:4.1_0": "corrupted by defilements,",
    "ai-thi-ap4:4.2_0": "... “I’ll sleep ...”",
    "ai-thi-ap4:4.3_0": "and live without defilements.",
    "ai-thi-ap4:5.1_0": "It was so welcome for me",
    "ai-thi-ap4:5.2_0": "... “I’ll sleep ...”",
    "ai-thi-ap4:5.3_0": "and fulfilled the Buddha’s instructions.",
    "ai-thi-ap4:6.1_0": "the four analytical knowledges,",
    "ai-thi-ap4:6.2_0": "... “I’ll sleep ...”",
    "ai-thi-ap4:6.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-thi-ap4:7.1_0": "That is how these verses were recited by the nun Paṅkadhā.",
    "ai-thi-ap4:8.1_0": "get the fourth Elder’s (and settlements),"
}