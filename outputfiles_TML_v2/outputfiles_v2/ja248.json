{
    "ai-ja248:0.1_0": "Jātaka",
    "ai-ja248:0.2_0": "Dukanipāta",
    "ai-ja248:0.3_0": "Siṅgālavagga",
    "ai-ja248:0.4_0": "8. Kiṁsukopamajātaka",
    "ai-ja248:1.1_0": "“What has you seen in regard to all?",
    "ai-ja248:1.2_0": "Transgression in this way",
    "ai-ja248:1.3_0": "because of all arrangements,",
    "ai-ja248:1.4_0": "of fine charioteer.”",
    "ai-ja248:2.1_0": "“Yes, all are",
    "ai-ja248:2.2_0": "Thoughts that are unmoved",
    "ai-ja248:2.3_0": "are unshakable in things,",
    "ai-ja248:2.4_0": "what’s your brother?”",
    "ai-ja248:3.1_0": "What is the eighth thing for an eon?"
}