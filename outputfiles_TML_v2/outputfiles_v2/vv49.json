{
    "ai-vv49:0.1_0": "Vimānavatthu",
    "ai-vv49:0.2_0": "Itthivimāna",
    "ai-vv49:0.3_0": "Mañjiṭṭhakavagga",
    "ai-vv49:0.4_0": "11. Vandanavimānavatthu",
    "ai-vv49:1.1_0": "Avoiding beauty",
    "ai-vv49:1.2_0": "Where you, your Majesty,",
    "ai-vv49:1.3_0": "lighting up the entire quarters.",
    "ai-vv49:1.4_0": "like a daughter-in-law of a low-worm,",
    "ai-vv49:2.1_0": "What is your beautiful complexion?",
    "ai-vv49:2.2_0": "... “I’ll sleep ...”",
    "ai-vv49:2.3_0": "so beautiful and ugly.”",
    "ai-vv49:3.1_0": "This is their delight,",
    "ai-vv49:3.2_0": "... “I’ll sleep ...”",
    "ai-vv49:3.3_0": "It is the fruit of that deed,",
    "ai-vv49:4.1_0": "“In human beings,",
    "ai-vv49:4.2_0": "When they see the ethical conduct,",
    "ai-vv49:4.3_0": "I paid homage to the Vedas:",
    "ai-vv49:4.4_0": "forty years I rose my hands.",
    "ai-vv49:5.1_0": "That’s why I praise",
    "ai-vv49:5.2_0": "... “I’ll sleep ...”",
    "ai-vv49:5.3_0": "and my complexion was like me.”",
    "ai-vv49:6.1_0": "entering the eleventh day."
}