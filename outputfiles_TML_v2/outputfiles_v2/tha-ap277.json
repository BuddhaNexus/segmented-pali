{
    "ai-tha-ap277:0.1_0": "Therāpadāna",
    "ai-tha-ap277:0.2_0": "Suvaṇṇabibbohanavagga",
    "ai-tha-ap277:0.3_0": "5 Ekañjalikattheraapadāna",
    "ai-tha-ap277:1.1_0": "“Having live in a lotus pond,",
    "ai-tha-ap277:1.2_0": "in leaf-hut, set out.",
    "ai-tha-ap277:1.3_0": "I have given a gift,",
    "ai-tha-ap277:1.4_0": "of the ascetic, the hermits,",
    "ai-tha-ap277:2.1_0": "Kassapa’s faculties.",
    "ai-tha-ap277:2.2_0": "the glorious protector of the world.",
    "ai-tha-ap277:2.3_0": "raising his hands in respectful salutation,",
    "ai-tha-ap277:2.4_0": "covering the flowers,",
    "ai-tha-ap277:3.1_0": "On the two occasions when the other goes forth",
    "ai-tha-ap277:3.2_0": "to make a flower,",
    "ai-tha-ap277:3.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap277:3.4_0": "this is the fruit.",
    "ai-tha-ap277:4.1_0": "I’ve completed the four quarters.",
    "ai-tha-ap277:4.2_0": "I became a fool,",
    "ai-tha-ap277:4.3_0": "A lame:",
    "ai-tha-ap277:4.4_0": "The Mahallika conversions",
    "ai-tha-ap277:5.1_0": "the four analytical knowledges,",
    "ai-tha-ap277:5.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap277:5.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap277:6.1_0": "That is how these verses were recited by the senior venerable Sañjaya.",
    "ai-tha-ap277:7.1_0": "for the fifth Elder Roja."
}