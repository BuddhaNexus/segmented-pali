{
    "ai-mil3.7.12:0.1_0": "Milindapañha",
    "ai-mil3.7.12:0.2_0": "Arūpadhammavavatthānavagga",
    "ai-mil3.7.12:0.3_0": "12. Samuddapañha",
    "ai-mil3.7.12:1.1_0": "The King said,",
    "ai-mil3.7.12:1.2_0": "“Venerable Sir, when a royal bull elephant is said to be ‘accuding’ a drop of water’.”",
    "ai-mil3.7.12:1.3_0": "The senior monk said,",
    "ai-mil3.7.12:1.4_0": "“Great king, the water jar that has been removed,",
    "ai-mil3.7.12:1.5_0": "one with water,",
    "ai-mil3.7.12:1.6_0": "That’s why it’s called the ‘factor’.”",
    "ai-mil3.7.12:2.1_0": "“Sir, has an army?”",
    "ai-mil3.7.12:3.1_0": "The twelfth enquiry,"
}