{
    "ai-tha-ap204:0.1_0": "Therāpadāna",
    "ai-tha-ap204:0.2_0": "Kaṇikārapupphiyavagga",
    "ai-tha-ap204:0.3_0": "Once again the Elder Tissa",
    "ai-tha-ap204:1.1_0": "“ color of gold, Blessed One!",
    "ai-tha-ap204:1.2_0": "mother-in-lawed,",
    "ai-tha-ap204:1.3_0": "shoots the walking-path,",
    "ai-tha-ap204:1.4_0": "with a mind full of love,",
    "ai-tha-ap204:2.1_0": "confident in heart,",
    "ai-tha-ap204:2.2_0": "I paid homage to the supreme knowledge.",
    "ai-tha-ap204:2.3_0": "Flame Tree flower",
    "ai-tha-ap204:2.4_0": "I enjoyed the Buddha.",
    "ai-tha-ap204:3.1_0": "It’s been thirty one eons",
    "ai-tha-ap204:3.2_0": "As a bird was worshipped by the flowers,",
    "ai-tha-ap204:3.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap204:3.4_0": "this fruit is to know,",
    "ai-tha-ap204:4.1_0": "Endowed with thirty-two marks",
    "ai-tha-ap204:4.2_0": "It’s good to enjoy a household,",
    "ai-tha-ap204:4.3_0": "endowed with the seven treasures,",
    "ai-tha-ap204:4.4_0": "The Mahallika conversions",
    "ai-tha-ap204:5.1_0": "the four analytical knowledges,",
    "ai-tha-ap204:5.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap204:5.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap204:6.1_0": "That is how these verses were recited by the senior venerable Scripture.",
    "ai-tha-ap204:7.1_0": "Seeing the second full flower"
}