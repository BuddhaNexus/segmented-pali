{
    "ai-vv42:0.1_0": "Vimānavatthu",
    "ai-vv42:0.2_0": "Itthivimāna",
    "ai-vv42:0.3_0": "Mañjiṭṭhakavagga",
    "ai-vv42:0.4_0": "4. Alomavimānavatthu",
    "ai-vv42:1.1_0": "Avoiding beauty",
    "ai-vv42:1.2_0": "Where you, your Majesty,",
    "ai-vv42:1.3_0": "lighting up the entire quarters.",
    "ai-vv42:1.4_0": "like a daughter-in-law of a low-worm,",
    "ai-vv42:2.1_0": "What is your beautiful complexion?",
    "ai-vv42:2.2_0": "... “I’ll sleep ...”",
    "ai-vv42:2.3_0": "so beautiful and ugly.”",
    "ai-vv42:3.1_0": "This is their delight,",
    "ai-vv42:3.2_0": "... “I’ll sleep ...”",
    "ai-vv42:3.3_0": "It is the fruit of that deed,",
    "ai-vv42:4.1_0": "“A Bārāṇasī, at Benares,",
    "ai-vv42:4.2_0": "from the Buddha, Kinsman of the Sun,",
    "ai-vv42:4.3_0": "I was plunging in the dry (saying):",
    "ai-vv42:4.4_0": "confident in the about",
    "ai-vv42:5.1_0": "discerning the training and the stainless,",
    "ai-vv42:5.2_0": "see the leper basket",
    "ai-vv42:5.3_0": "see the shake,",
    "ai-vv42:5.4_0": "who can’t do the merits?",
    "ai-vv42:6.1_0": "That’s why I praise",
    "ai-vv42:6.2_0": "... “I’ll sleep ...”",
    "ai-vv42:6.3_0": "and my complexion was like me.”",
    "ai-vv42:7.1_0": "fine lady, stainless, and fourth."
}