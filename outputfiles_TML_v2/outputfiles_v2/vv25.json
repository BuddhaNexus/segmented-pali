{
    "ai-vv25:0.1_0": "Vimānavatthu",
    "ai-vv25:0.2_0": "Itthivimāna",
    "ai-vv25:0.3_0": "Cittalatāvagga",
    "ai-vv25:0.4_0": "8. Niddāvimānavatthu",
    "ai-vv25:1.1_0": "Avoiding beauty",
    "ai-vv25:1.2_0": "Where you, your Majesty,",
    "ai-vv25:1.3_0": "lighting up the entire quarters.",
    "ai-vv25:1.4_0": "like a daughter-in-law of a low-worm,",
    "ai-vv25:2.1_0": "What is your beautiful complexion?",
    "ai-vv25:2.2_0": "... “I’ll sleep ...”",
    "ai-vv25:2.3_0": "so beautiful and ugly.”",
    "ai-vv25:3.1_0": "This is their delight,",
    "ai-vv25:3.2_0": "... “I’ll sleep ...”",
    "ai-vv25:3.3_0": "It is the fruit of that deed,",
    "ai-vv25:4.1_0": "“He does give me another life,",
    "ai-vv25:4.2_0": "a female lay follower",
    "ai-vv25:4.3_0": "faithful, accomplished in ethics,",
    "ai-vv25:4.4_0": "restraint, and constraint,",
    "ai-vv25:5.1_0": "food and she’s given,",
    "ai-vv25:5.2_0": "fermented urine.",
    "ai-vv25:5.3_0": "When you’ve discarded the straight ground,",
    "ai-vv25:5.4_0": "with clear and confident heart,",
    "ai-vv25:6.1_0": "I rejoice in the four quarters.",
    "ai-vv25:6.2_0": "would observe the sabbath,",
    "ai-vv25:6.3_0": "who leads the spiritual life",
    "ai-vv25:6.4_0": "on the fourteenth and the fifteenth days,",
    "ai-vv25:7.1_0": "and the eighth day of the fortnight,",
    "ai-vv25:7.2_0": "restrained in the precepts,",
    "ai-vv25:7.3_0": "of restraint and sharing,",
    "ai-vv25:7.4_0": "I’m the one who understands",
    "ai-vv25:8.1_0": "He refrains from killing living creatures,",
    "ai-vv25:8.2_0": "lying,",
    "ai-vv25:8.3_0": "those things love and enjoy.",
    "ai-vv25:8.4_0": "avoid a care of the rod,",
    "ai-vv25:9.1_0": "Pañcasikha had developed the five wives.",
    "ai-vv25:9.2_0": "who see the noble truths.",
    "ai-vv25:9.3_0": "the eye from the laywomen.",
    "ai-vv25:9.4_0": "of the renowned Gotama",
    "ai-vv25:10.1_0": "That’s why I praise",
    "ai-vv25:10.2_0": "... “I’ll sleep ...”",
    "ai-vv25:10.3_0": "and my complexion was like me.”",
    "ai-vv25:11.1_0": "eighth was simpled."
}