{
    "ai-thig2.6:0.1_0": "Therīgāthā",
    "ai-thig2.6:0.2_0": "Dukanipāta",
    "ai-thig2.6:0.3_0": "6. Mettikātherīgāthā",
    "ai-thig2.6:1.1_0": "Though in pain,",
    "ai-thig2.6:1.2_0": "feeble, my youth long gone,",
    "ai-thig2.6:1.3_0": "I climb the mountain,",
    "ai-thig2.6:1.4_0": "I climbed the mountain!",
    "ai-thig2.6:2.1_0": "Having laid down my outer robe",
    "ai-thig2.6:2.2_0": "Having attained the bowl,",
    "ai-thig2.6:2.3_0": "sitting on a rock,",
    "ai-thig2.6:2.4_0": "Then my mind was freed—",
    "ai-thig2.6:2.5_0": "I’ve attained the three knowledges;",
    "ai-thig2.6:2.6_0": "and fulfilled the Buddha’s instructions.",
    "ai-thig2.6:3.1_0": "love ..."
}