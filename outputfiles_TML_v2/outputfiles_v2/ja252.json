{
    "ai-ja252:0.1_0": "Jātaka",
    "ai-ja252:0.2_0": "Tikanipāta",
    "ai-ja252:0.3_0": "Saṅkappavagga",
    "ai-ja252:0.4_0": "2. Tilamuṭṭhijātaka",
    "ai-ja252:1.1_0": "“I am the one who gives attention to it,",
    "ai-ja252:1.2_0": "that you with the leaf-hut.",
    "ai-ja252:1.3_0": "Taking me by the arm,",
    "ai-ja252:1.4_0": "by a progress.",
    "ai-ja252:2.1_0": "you won’t delight in life.",
    "ai-ja252:2.2_0": "that you’re a brahmin by birth.",
    "ai-ja252:2.3_0": "Taking me by the arm,",
    "ai-ja252:2.4_0": "Proclaimed three times.”",
    "ai-ja252:3.1_0": "“They are unintelligent,",
    "ai-ja252:3.2_0": "The one who stops them with a stick",
    "ai-ja252:3.3_0": "the instructions of the Buddha,",
    "ai-ja252:3.4_0": "so the astute will know.”",
    "ai-ja252:4.1_0": "The second sub-story (burning) for the second sesame."
}