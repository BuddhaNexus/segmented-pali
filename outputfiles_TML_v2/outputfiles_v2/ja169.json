{
    "ai-ja169:0.1_0": "Jātaka",
    "ai-ja169:0.2_0": "Dukanipāta",
    "ai-ja169:0.3_0": "Santhavavagga",
    "ai-ja169:0.4_0": "9. Arakajātaka",
    "ai-ja169:1.1_0": "Since those who have a mind of love",
    "ai-ja169:1.2_0": "out of compassion for the whole world.",
    "ai-ja169:1.3_0": "above, below, and all around;",
    "ai-ja169:1.4_0": "having entirely measureless food,",
    "ai-ja169:2.1_0": "My mind is limitless,",
    "ai-ja169:2.2_0": "and gradually consolidated",
    "ai-ja169:2.3_0": "any deeds they have done.",
    "ai-ja169:2.4_0": "Unable to live.”",
    "ai-ja169:3.1_0": "ninth pair of foliage"
}