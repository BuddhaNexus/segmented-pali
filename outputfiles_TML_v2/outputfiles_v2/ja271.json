{
    "ai-ja271:0.1_0": "Jātaka",
    "ai-ja271:0.2_0": "Tikanipāta",
    "ai-ja271:0.3_0": "Udapānavagga",
    "ai-ja271:0.4_0": "1. Udapānadūsakajātaka",
    "ai-ja271:1.1_0": "“In the wilderness, in the sage’s hermitage,",
    "ai-ja271:1.2_0": "for a long time, Tapassī!",
    "ai-ja271:1.3_0": "is hard to find again.",
    "ai-ja271:1.4_0": "How did he make",
    "ai-ja271:2.1_0": "“This is the Dhamma Sigālaka for the sake of illness,",
    "ai-ja271:2.2_0": "I drunk on my army,",
    "ai-ja271:2.3_0": "I also am the Dhamma,",
    "ai-ja271:2.4_0": "you won’t be satisfied with you.”",
    "ai-ja271:3.1_0": "“Those who see such Truth",
    "ai-ja271:3.2_0": "What kind of thing?",
    "ai-ja271:3.3_0": "Don't give up what is contrary to the Teaching,",
    "ai-ja271:3.4_0": "when I saw her coming.",
    "ai-ja271:4.1_0": "The thirteen nearbys"
}