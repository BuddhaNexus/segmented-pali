{
    "ai-tha-ap220:0.1_0": "Therāpadāna",
    "ai-tha-ap220:0.2_0": "Hatthivagga",
    "ai-tha-ap220:0.3_0": "Queen Sectary",
    "ai-tha-ap220:1.1_0": "“Having taken a basket,",
    "ai-tha-ap220:1.2_0": "I used to be his preceptor,",
    "ai-tha-ap220:1.3_0": "I will speak to you,",
    "ai-tha-ap220:1.4_0": "ties, and underlying tendencies—",
    "ai-tha-ap220:2.1_0": "I saw the stainless Buddha",
    "ai-tha-ap220:2.2_0": "receiver of sacrifices,",
    "ai-tha-ap220:2.3_0": "A noble leader’s the best,",
    "ai-tha-ap220:2.4_0": "the Buddha, the best of men,",
    "ai-tha-ap220:3.1_0": "spread out a bowl.",
    "ai-tha-ap220:3.2_0": "Out of compassion for the Supreme One,",
    "ai-tha-ap220:3.3_0": "like a great hero, the great hero,",
    "ai-tha-ap220:3.4_0": "in the world’s elder.",
    "ai-tha-ap220:4.1_0": "Seeing the light of the world,",
    "ai-tha-ap220:4.2_0": "My stainless moon,",
    "ai-tha-ap220:4.3_0": "I bow to your feet,",
    "ai-tha-ap220:4.4_0": "with clear and confident heart,",
    "ai-tha-ap220:5.1_0": "if he makes his four eons;",
    "ai-tha-ap220:5.2_0": "took ahold of the hand",
    "ai-tha-ap220:5.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap220:5.4_0": "this fruit is the leper ball.",
    "ai-tha-ap220:6.1_0": "It’s been seven eons",
    "ai-tha-ap220:6.2_0": "solitary, content,",
    "ai-tha-ap220:6.3_0": "The Sun, however, Sunakkhatta,",
    "ai-tha-ap220:6.4_0": "The Mahallika conversions",
    "ai-tha-ap220:7.1_0": "the four analytical knowledges,",
    "ai-tha-ap220:7.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap220:7.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap220:8.1_0": "That is how these verses were recited by the senior venerable Abhibhū.",
    "ai-tha-ap220:9.1_0": "Understanding these eight Elders"
}