{
    "ai-tha-ap436:0.1_0": "Therāpadāna",
    "ai-tha-ap436:0.2_0": "Ekavihārivagga",
    "ai-tha-ap436:0.3_0": "4. Ñāṇatthavikattheraapadāna",
    "ai-tha-ap436:1.1_0": "“Marked like a turtle;",
    "ai-tha-ap436:1.2_0": "it lights a lamp,",
    "ai-tha-ap436:1.3_0": "don’t see what you wish,",
    "ai-tha-ap436:1.4_0": "I saw the supreme landing.",
    "ai-tha-ap436:2.1_0": "apart from the water’s distance.",
    "ai-tha-ap436:2.2_0": "A vow’s snare,",
    "ai-tha-ap436:2.3_0": "to definitely unshakable,",
    "ai-tha-ap436:2.4_0": "I have attained the highest goal.",
    "ai-tha-ap436:3.1_0": "‘Sometimes in darkness,",
    "ai-tha-ap436:3.2_0": "they fall from a net of delusion.",
    "ai-tha-ap436:3.3_0": "giveing knowledge,",
    "ai-tha-ap436:3.4_0": "he’s lying down, like a great scatter.",
    "ai-tha-ap436:4.1_0": "for this world is like the ocean.",
    "ai-tha-ap436:4.2_0": "Uttara, expert in every way.",
    "ai-tha-ap436:4.3_0": "you have no knowledge about them.",
    "ai-tha-ap436:4.4_0": "as far as the earth extends;",
    "ai-tha-ap436:5.1_0": "by that knowledge and insight:",
    "ai-tha-ap436:5.2_0": "This is called the Buddha.",
    "ai-tha-ap436:5.3_0": "I worship the great hero,",
    "ai-tha-ap436:5.4_0": "always straying.",
    "ai-tha-ap436:6.1_0": "it’s a hundred thousand eons",
    "ai-tha-ap436:6.2_0": "I have attained the highest goal.",
    "ai-tha-ap436:6.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap436:6.4_0": "you’ve shelled the fruit.",
    "ai-tha-ap436:7.1_0": "corrupted by defilements,",
    "ai-tha-ap436:7.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap436:7.3_0": "and live without defilements.",
    "ai-tha-ap436:8.1_0": "It was so welcome for me",
    "ai-tha-ap436:8.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap436:8.3_0": "and fulfilled the Buddha’s instructions.",
    "ai-tha-ap436:9.1_0": "the four analytical knowledges,",
    "ai-tha-ap436:9.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap436:9.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap436:10.1_0": "That is how these verses were recited by the senior venerable Scriptures.",
    "ai-tha-ap436:11.1_0": "to the fourth Elder Nun Knowledge, Understanding the fourth section,"
}