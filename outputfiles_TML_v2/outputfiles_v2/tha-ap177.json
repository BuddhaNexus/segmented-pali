{
    "ai-tha-ap177:0.1_0": "Therāpadāna",
    "ai-tha-ap177:0.2_0": "Kumudavagga",
    "ai-tha-ap177:0.3_0": "The Elder Nun Sīvaka",
    "ai-tha-ap177:1.1_0": "“I’m extinguished in the world,",
    "ai-tha-ap177:1.2_0": "if it is on the bank of the Vaggumudā;",
    "ai-tha-ap177:1.3_0": "confident in heart,",
    "ai-tha-ap177:1.4_0": "I’m sitting in silence,",
    "ai-tha-ap177:2.1_0": "on the bank of the Gandhamādana,",
    "ai-tha-ap177:2.2_0": "For the bliss of here and now,",
    "ai-tha-ap177:2.3_0": "Having worshipped that seat,",
    "ai-tha-ap177:2.4_0": "Extinguishment is very blameworthy.",
    "ai-tha-ap177:3.1_0": "confident in heart,",
    "ai-tha-ap177:3.2_0": "I paid homage to the supreme Buddha.",
    "ai-tha-ap177:3.3_0": "A hundred thousand aeons",
    "ai-tha-ap177:3.4_0": "Would I have a bad destination?",
    "ai-tha-ap177:4.1_0": "“Over a thousand coins,",
    "ai-tha-ap177:4.2_0": "for countless eons.",
    "ai-tha-ap177:4.3_0": "plundering the dancing and rolling on,",
    "ai-tha-ap177:4.4_0": "a wheel-turning monarch.",
    "ai-tha-ap177:5.1_0": "the four analytical knowledges,",
    "ai-tha-ap177:5.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap177:5.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap177:6.1_0": "That is how these verses were recited by the senior venerable Sīha.",
    "ai-tha-ap177:7.1_0": "The fifth Elder Nun Sīvaka"
}