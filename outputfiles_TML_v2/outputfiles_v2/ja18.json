{
    "ai-ja18:0.1_0": "Jātaka",
    "ai-ja18:0.2_0": "Ekakanipāta",
    "ai-ja18:0.3_0": "Sīlavagga",
    "ai-ja18:0.4_0": "8. Matakabhattajātaka",
    "ai-ja18:1.1_0": "“If sentient beings only knew",
    "ai-ja18:1.2_0": "this birth is suffering,",
    "ai-ja18:1.3_0": "You can’t kill living creatures,",
    "ai-ja18:1.4_0": "for they grieve for killing living creatures.”",
    "ai-ja18:2.1_0": "The eighth section for recitation on the clay balls."
}