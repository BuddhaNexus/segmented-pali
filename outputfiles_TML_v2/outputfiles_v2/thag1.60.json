{
    "ai-thag1.60:0.1_0": "Theragāthā",
    "ai-thag1.60:0.2_0": "Ekakanipāta",
    "ai-thag1.60:0.3_0": "Chaṭṭhavagga",
    "ai-thag1.60:0.4_0": "1.60. Sīvalittheragāthā",
    "ai-thag1.60:1.1_0": "My wishes—the purpose I had",
    "ai-thag1.60:1.2_0": "for entering this hut—came true.",
    "ai-thag1.60:1.3_0": "Abandoning the tendency to conceit,",
    "ai-thag1.60:1.4_0": "I’ll realize knowledge and liberation.",
    "ai-thag1.60:1.5_0": "…",
    "ai-thag1.60:1.6_0": "Sīvali ...",
    "ai-thag1.60:1.7_0": "The sixth sub-chapter on lying down is finished.",
    "ai-thag1.60:2.0_0": "“This is the summary:",
    "ai-thag1.60:2.1_0": "Godhika, and Subhā,",
    "ai-thag1.60:2.2_0": "The hermits called Isisiṅga",
    "ai-thag1.60:2.3_0": "the Elders, Vaḍḍha,",
    "ai-thag1.60:2.4_0": "Taking a little hut,",
    "ai-thag1.60:2.5_0": "delightful, Kuṭikaṇṇa,",
    "ai-thag1.60:2.6_0": "The Sīvaka Sīha"
}