{
    "ai-ja379:0.1_0": "Jātaka",
    "ai-ja379:0.2_0": "Chakkanipāta",
    "ai-ja379:0.3_0": "Avāriyavagga",
    "ai-ja379:0.4_0": "4. Nerujātaka",
    "ai-ja379:1.1_0": "“Gentlemen, Kāka, and Aṅkicca,",
    "ai-ja379:1.2_0": "than breath of the past.",
    "ai-ja379:1.3_0": "May you be like me,",
    "ai-ja379:1.4_0": "to this mountain:",
    "ai-ja379:2.1_0": "Firstly, a lion, a tiger.",
    "ai-ja379:2.2_0": "like the jackals of Migāra’s mother,",
    "ai-ja379:2.3_0": "although everyone defeated in this way,",
    "ai-ja379:2.4_0": "This is the going forth!”",
    "ai-ja379:3.1_0": "“This is their simile,",
    "ai-ja379:3.2_0": "for human beings.",
    "ai-ja379:3.3_0": "in this life, beauty, happiness,",
    "ai-ja379:3.4_0": "they live for the opportunity.”",
    "ai-ja379:4.1_0": "“Whereever comes along,",
    "ai-ja379:4.2_0": "When your mind is peaceful and stable,",
    "ai-ja379:4.3_0": "Or the great hermits.",
    "ai-ja379:4.4_0": "if you don’t stay;",
    "ai-ja379:5.1_0": "Where the lazy and clever,",
    "ai-ja379:5.2_0": "flies, and troops,",
    "ai-ja379:5.3_0": "There are no peaceful ones there,",
    "ai-ja379:5.4_0": "if it is not a particular;",
    "ai-ja379:6.1_0": "The world can’t explain this.",
    "ai-ja379:6.2_0": "“These are the low ones.",
    "ai-ja379:6.3_0": "make no distinction,",
    "ai-ja379:6.4_0": "Let’s leave the Nerañjara.”",
    "ai-ja379:7.1_0": "It is the fourth (birth) birth."
}