{
    "ai-ja291:0.1_0": "Jātaka",
    "ai-ja291:0.2_0": "Tikanipāta",
    "ai-ja291:0.3_0": "Kumbhavagga",
    "ai-ja291:0.4_0": "1. Surāghaṭajātaka",
    "ai-ja291:1.1_0": "“After going to the charnel ground,",
    "ai-ja291:1.2_0": "I’ve gained the ascetic practices,",
    "ai-ja291:1.3_0": "the exclusiveness.",
    "ai-ja291:1.4_0": "for that long the happiness of bliss",
    "ai-ja291:2.1_0": "When it’s lying dead,",
    "ai-ja291:2.2_0": "Past lives in a pot,",
    "ai-ja291:2.3_0": "Then naked and ate,",
    "ai-ja291:2.4_0": "like a fool frets.",
    "ai-ja291:3.1_0": "Just so, the one who receives wealth,",
    "ai-ja291:3.2_0": "if she uses it when she is negligent;",
    "ai-ja291:3.3_0": "the fool suffers,",
    "ai-ja291:3.4_0": "scoundrels on a wall.”",
    "ai-ja291:4.1_0": "First you’re well born."
}