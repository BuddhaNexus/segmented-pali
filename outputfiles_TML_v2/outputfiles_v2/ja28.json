{
    "ai-ja28:0.1_0": "Jātaka",
    "ai-ja28:0.2_0": "Ekakanipāta",
    "ai-ja28:0.3_0": "Kuruṅgavagga",
    "ai-ja28:0.4_0": "8. Nandivisālajātaka",
    "ai-ja28:1.1_0": "“Speak only what you say,",
    "ai-ja28:1.2_0": "what’s known as the Teacher’s words?",
    "ai-ja28:1.3_0": "speaking Dhamma to another:",
    "ai-ja28:1.4_0": "heavy burden,",
    "ai-ja28:1.5_0": "And he obtained wealth;",
    "ai-ja28:1.6_0": "And he was delighted with that.”",
    "ai-ja28:2.1_0": "she was eighth day of the fortnight,"
}