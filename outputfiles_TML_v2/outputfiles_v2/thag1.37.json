{
    "ai-thag1.37:0.1_0": "Theragāthā",
    "ai-thag1.37:0.2_0": "Ekakanipāta",
    "ai-thag1.37:0.3_0": "Catutthavagga",
    "ai-thag1.37:0.4_0": "Kuṭikaputta",
    "ai-thag1.37:1.1_0": "Some travel to different countries,",
    "ai-thag1.37:1.2_0": "wandering undisciplined.",
    "ai-thag1.37:1.3_0": "If they lose their meditation,",
    "ai-thag1.37:1.4_0": "what will such rotten conduct achieve?",
    "ai-thag1.37:1.5_0": "So you should try to aggression,",
    "ai-thag1.37:1.6_0": "practicing absorption undistracted.",
    "ai-thag1.37:1.7_0": "the senior monk Kumāra’s mother ..."
}