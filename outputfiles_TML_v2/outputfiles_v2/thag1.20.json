{
    "ai-thag1.20:0.1_0": "Theragāthā",
    "ai-thag1.20:0.2_0": "Ekakanipāta",
    "ai-thag1.20:0.3_0": "Dutiyavagga",
    "ai-thag1.20:0.4_0": "1.20. Ajitattheragāthā",
    "ai-thag1.20:1.1_0": "“There is no fear for me,",
    "ai-thag1.20:1.2_0": "nor do I long for life.",
    "ai-thag1.20:1.3_0": "I will lay down the jungle.",
    "ai-thag1.20:1.4_0": "aware and mindful.",
    "ai-thag1.20:1.5_0": "…",
    "ai-thag1.20:1.6_0": "The senior monk Ajita,",
    "ai-thag1.20:1.7_0": "The second sub-chapter on lying down is finished.",
    "ai-thag1.20:2.0_0": "“This is the summary:",
    "ai-thag1.20:2.1_0": "The Great Cūḷapanthaka is like a huge box;",
    "ai-thag1.20:2.2_0": "like a lion in a jungle thicket.",
    "ai-thag1.20:2.3_0": "Kuṇḍalakesā, and Belaṭṭhiputta,",
    "ai-thag1.20:2.4_0": "Bondservants,",
    "ai-thag1.20:2.5_0": "The elder Siṅgālaka",
    "ai-thag1.20:2.6_0": "And who’s unqualified:"
}