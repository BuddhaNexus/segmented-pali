{
    "ai-tha-ap283:0.1_0": "Therāpadāna",
    "ai-tha-ap283:0.2_0": "Paṇṇadāyakavagga",
    "ai-tha-ap283:0.3_0": "1. Paṇṇadāyakattheraapadāna",
    "ai-tha-ap283:1.1_0": "while sitting on his ears,",
    "ai-tha-ap283:1.2_0": "eating in moderation,",
    "ai-tha-ap283:1.3_0": "I thinked my peaceful position,",
    "ai-tha-ap283:1.4_0": "took a great uproar,",
    "ai-tha-ap283:2.1_0": "they’re prosperous and full of the world,",
    "ai-tha-ap283:2.2_0": "healer of the whole world.",
    "ai-tha-ap283:2.3_0": "to give it a gift",
    "ai-tha-ap283:2.4_0": "sat in the leaf-hut.",
    "ai-tha-ap283:3.1_0": "if he makes his four eons;",
    "ai-tha-ap283:3.2_0": "and then the leaf-hut.",
    "ai-tha-ap283:3.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap283:3.4_0": "aspiring to the leaf-hut.",
    "ai-tha-ap283:4.1_0": "the four analytical knowledges,",
    "ai-tha-ap283:4.2_0": "having excellence and liberation,",
    "ai-tha-ap283:4.3_0": "I’ve realized the six kinds of direct knowledge,",
    "ai-tha-ap283:4.4_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap283:5.1_0": "That is how these verses were recited by the senior venerable Puṇṇaka.",
    "ai-tha-ap283:6.1_0": "First the Elder Paṇṇaka"
}