{
    "ai-ja186:0.1_0": "Jātaka",
    "ai-ja186:0.2_0": "Dukanipāta",
    "ai-ja186:0.3_0": "Asadisavagga",
    "ai-ja186:0.4_0": "6. Dadhivāhanajātaka",
    "ai-ja186:1.1_0": "“Smells like an ointment, which is sweet,",
    "ai-ja186:1.2_0": "in the past Ambaṭṭha.",
    "ai-ja186:1.3_0": "then gain the one heedless.",
    "ai-ja186:1.4_0": "What mango fruits are there to be in the public?”",
    "ai-ja186:2.1_0": "“When crooked around,",
    "ai-ja186:2.2_0": "curd from the mango grove",
    "ai-ja186:2.3_0": "roots that are to be found",
    "ai-ja186:2.4_0": "branches, trampled down,",
    "ai-ja186:2.5_0": "It’s like living with bad people;",
    "ai-ja186:2.6_0": "That’s why Ambaṭṭhaka fruits.”",
    "ai-ja186:3.1_0": "curd on the ground,"
}