{
    "ai-thig3.8:0.1_0": "Therīgāthā",
    "ai-thig3.8:0.2_0": "Tikanipāta",
    "ai-thig3.8:0.3_0": "8. Somātherīgāthā",
    "ai-thig3.8:1.1_0": "“That place",
    "ai-thig3.8:1.2_0": "it’s for the sages to attain.",
    "ai-thig3.8:1.3_0": "Now that with a two-finger wisdom",
    "ai-thig3.8:1.4_0": "with her two-fingered wisdom.”",
    "ai-thig3.8:2.1_0": "“What difference does womanhood make",
    "ai-thig3.8:2.2_0": "when the mind is serene,",
    "ai-thig3.8:2.3_0": "When knowledges exist,",
    "ai-thig3.8:2.4_0": "as they rightly discern the Dhamma.”",
    "ai-thig3.8:3.1_0": "undefeated everywhere,",
    "ai-thig3.8:3.2_0": "and the mass of darkness is shattered.",
    "ai-thig3.8:3.3_0": "So know this, Wicked One:",
    "ai-thig3.8:3.4_0": "you’re beaten, terminator!”",
    "ai-thig3.8:4.1_0": "…",
    "ai-thig3.8:4.2_0": "senior monks ...",
    "ai-thig3.8:5.1_0": "The Book of the Threes is finished."
}