{
    "ai-ja281:0.1_0": "Jātaka",
    "ai-ja281:0.2_0": "Tikanipāta",
    "ai-ja281:0.3_0": "Abbhantaravagga",
    "ai-ja281:0.4_0": "1. Abbhantarajātaka",
    "ai-ja281:1.1_0": "“Fine! A bad declaration:",
    "ai-ja281:1.2_0": "than the fruit of good deeds.",
    "ai-ja281:1.3_0": "having eaten a misfortune Nāḷāgiri,",
    "ai-ja281:1.4_0": "a wheel-turning monarch.",
    "ai-ja281:2.1_0": "you also, Bhaddā,",
    "ai-ja281:2.2_0": "as she was heedless,",
    "ai-ja281:2.3_0": "he said to the King: “Go you, King, your son will bring you one.”",
    "ai-ja281:2.4_0": "this is the simile of the fruit.”",
    "ai-ja281:3.1_0": "“Throw out the household life,",
    "ai-ja281:3.2_0": "if you get better.",
    "ai-ja281:3.3_0": "Dispensation better than oneself,",
    "ai-ja281:3.4_0": "they will get enough rain.”",
    "ai-ja281:4.1_0": "with goads others first."
}