    "ai-thag1.26:0.1_0": "Theragāthā",
    "ai-thag1.26:0.2_0": "Ekakanipāta",
    "ai-thag1.26:0.3_0": "Tatiyavagga",
    "ai-thag1.26:0.4_0": "1.26. Abhayattheragāthā",
    "ai-thag1.26:1.1_0": "Having heard the fine words,",
    "ai-thag1.26:1.2_0": "from the Buddha, Kinsman of the Sun,",
    "ai-thag1.26:1.3_0": "I penetrated the subtle truth,",
    "ai-thag1.26:1.4_0": "like a hair-tip with an arrow.",
    "ai-thag1.26:1.5_0": "... senior monks ..."
