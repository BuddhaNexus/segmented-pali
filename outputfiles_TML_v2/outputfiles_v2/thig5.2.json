{
    "ai-thig5.2:0.1_0": "Therīgāthā",
    "ai-thig5.2:0.2_0": "Pañcakanipāta",
    "ai-thig5.2:0.3_0": "2. Vimalātherīgāthā",
    "ai-thig5.2:1.1_0": "Intoxicated by my appearance,",
    "ai-thig5.2:1.2_0": "my figure, my beauty, my fame,",
    "ai-thig5.2:1.3_0": "and owing to my youth,",
    "ai-thig5.2:1.4_0": "I despised other women.",
    "ai-thig5.2:2.1_0": "I adorned this body,",
    "ai-thig5.2:2.2_0": "so fancy, cooed over by fools,",
    "ai-thig5.2:2.3_0": "and stood at the brothel door,",
    "ai-thig5.2:2.4_0": "like a hunter laying a snare.",
    "ai-thig5.2:3.1_0": "I stripped for them,",
    "ai-thig5.2:3.2_0": "revealing my many hidden treasures.",
    "ai-thig5.2:3.3_0": "Creating an intricate illusion,",
    "ai-thig5.2:3.4_0": "I laughed, teasing those men.",
    "ai-thig5.2:4.1_0": "Having wandered for alms,",
    "ai-thig5.2:4.2_0": "my head is shaven, I wear the outer robe.",
    "ai-thig5.2:4.3_0": "I sat at the root of a tree to meditate;",
    "ai-thig5.2:4.4_0": "I've gained freedom from thought.",
    "ai-thig5.2:5.1_0": "All bonds are cut off,",
    "ai-thig5.2:5.2_0": "whether human or divine.",
    "ai-thig5.2:5.3_0": "and having destroyed all defilements,",
    "ai-thig5.2:5.4_0": "I have become cooled and quenched.",
    "ai-thig5.2:6.1_0": "a former senior nun ..."
}