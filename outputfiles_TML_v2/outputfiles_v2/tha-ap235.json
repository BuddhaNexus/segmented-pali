{
    "ai-tha-ap235:0.1_0": "Therāpadāna",
    "ai-tha-ap235:0.2_0": "Udakāsanavagga",
    "ai-tha-ap235:0.3_0": "3. Sālapupphiyattheraapadāna",
    "ai-tha-ap235:1.1_0": "“One from a mountain cave,",
    "ai-tha-ap235:1.2_0": "Pūraṇa Kassapa.",
    "ai-tha-ap235:1.3_0": "while walking along the doorway,",
    "ai-tha-ap235:1.4_0": "I saw a variety of traveling,",
    "ai-tha-ap235:2.1_0": "Awakened One’s bowl,",
    "ai-tha-ap235:2.2_0": "sal flowering buds,",
    "ai-tha-ap235:2.3_0": "rightly freed through the highest Buddha.",
    "ai-tha-ap235:2.4_0": "with clear and confident heart,",
    "ai-tha-ap235:3.1_0": "It’s been thirty one eons",
    "ai-tha-ap235:3.2_0": "Anotatta attacks",
    "ai-tha-ap235:3.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap235:3.4_0": "as a flower in a sal tree.",
    "ai-tha-ap235:4.1_0": "I’ve completed the four quarters.",
    "ai-tha-ap235:4.2_0": "his joined palms were full.",
    "ai-tha-ap235:4.3_0": "endowed with the seven treasures,",
    "ai-tha-ap235:4.4_0": "The Mahallika conversions",
    "ai-tha-ap235:5.1_0": "the four analytical knowledges,",
    "ai-tha-ap235:5.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap235:5.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap235:6.1_0": "That is how these verses were recited by the senior venerable Sāla.",
    "ai-tha-ap235:7.1_0": "but the third time the Elder Uppalavaṇṇā,"
}