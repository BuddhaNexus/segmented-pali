{
    "ai-thig5.7:0.1_0": "Therīgāthā",
    "ai-thig5.7:0.2_0": "Pañcakanipāta",
    "ai-thig5.7:0.3_0": "7. Sakulātherīgāthā",
    "ai-thig5.7:1.1_0": "While staying at home",
    "ai-thig5.7:1.2_0": "I heard the teaching from a mendicant.",
    "ai-thig5.7:1.3_0": "I saw the stainless Dhamma,",
    "ai-thig5.7:1.4_0": "extinguishment, the imperishable state.",
    "ai-thig5.7:2.1_0": "I am his son and daughter.",
    "ai-thig5.7:2.2_0": "and clothes.",
    "ai-thig5.7:2.3_0": "I cut off my hair and beard cut off,",
    "ai-thig5.7:2.4_0": "I went forth to homelessness.",
    "ai-thig5.7:3.1_0": "As a trainee nun,",
    "ai-thig5.7:3.2_0": "I developed the direct path.",
    "ai-thig5.7:3.3_0": "I gave up greed and hate,",
    "ai-thig5.7:3.4_0": "along with associated defilements.",
    "ai-thig5.7:4.1_0": "When I was fully ordained as a nun,",
    "ai-thig5.7:4.2_0": "I recollected my past lives,",
    "ai-thig5.7:4.3_0": "my clairvoyance is purified,",
    "ai-thig5.7:4.4_0": "immaculate and fully developed.",
    "ai-thig5.7:5.1_0": "Conditions are born of causes, crumbling;",
    "ai-thig5.7:5.2_0": "having seen them as other,",
    "ai-thig5.7:5.3_0": "I gave up all defilements,",
    "ai-thig5.7:5.4_0": "I have become cooled and quenched.",
    "ai-thig5.7:6.1_0": ", Sakulā,"
}