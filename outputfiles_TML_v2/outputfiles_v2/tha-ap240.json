{
    "ai-tha-ap240:0.1_0": "Therāpadāna",
    "ai-tha-ap240:0.2_0": "Udakāsanavagga",
    "ai-tha-ap240:0.3_0": "The Elder Queen Anulā",
    "ai-tha-ap240:1.1_0": "“Friends in a craft,",
    "ai-tha-ap240:1.2_0": "I went to the ocher robe,",
    "ai-tha-ap240:1.3_0": "seeing the Buddha,",
    "ai-tha-ap240:1.4_0": "I crushed the mango grove.",
    "ai-tha-ap240:2.1_0": "A single eon irrevers:",
    "ai-tha-ap240:2.2_0": "Giving a gift (of giving) gifts,",
    "ai-tha-ap240:2.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap240:2.4_0": "this mango fruits the wood,",
    "ai-tha-ap240:3.1_0": "the four analytical knowledges,",
    "ai-tha-ap240:3.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap240:3.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap240:4.1_0": "That is how these verses were recited by the senior venerable Ambaṭṭha.",
    "ai-tha-ap240:5.1_0": "for the eighth India (whole) the Elders’ rose-apple trees"
}