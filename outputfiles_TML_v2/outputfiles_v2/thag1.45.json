{
    "ai-thag1.45:0.1_0": "Theragāthā",
    "ai-thag1.45:0.2_0": "Ekakanipāta",
    "ai-thag1.45:0.3_0": "Pañcamavagga",
    "ai-thag1.45:0.4_0": "delightful is the side of the Kattika Sutta",
    "ai-thag1.45:1.1_0": "Just as a fine thoroughbred,",
    "ai-thag1.45:1.2_0": "it soon stands firm again.",
    "ai-thag1.45:1.3_0": "Even so is one accomplished in vision,",
    "ai-thag1.45:1.4_0": "a disciple of the Buddha.",
    "ai-thag1.45:1.5_0": "Ramaṇī ..."
}