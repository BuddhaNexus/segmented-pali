{
    "ai-thag2.48:0.1_0": "Theragāthā",
    "ai-thag2.48:0.2_0": "Dukanipāta",
    "ai-thag2.48:0.3_0": "Pañcamavagga",
    "ai-thag2.48:0.4_0": "2.48. Vajjitattheragāthā",
    "ai-thag2.48:1.1_0": "Transmigrating for such a long time,",
    "ai-thag2.48:1.2_0": "doesn’t move regularly.",
    "ai-thag2.48:1.3_0": "not seeing the noble truths,",
    "ai-thag2.48:1.4_0": "a blind, unenlightened person.",
    "ai-thag2.48:2.1_0": "Being diligent,",
    "ai-thag2.48:2.2_0": "transmigration was unbound.",
    "ai-thag2.48:2.3_0": "All states of rebirth are cut off;",
    "ai-thag2.48:2.4_0": "now there’ll be no more future lives.”",
    "ai-thag2.48:2.5_0": "... a senior monk ..."
}