{
    "ai-thag2.21:0.1_0": "Theragāthā",
    "ai-thag2.21:0.2_0": "Dukanipāta",
    "ai-thag2.21:0.3_0": "Tatiyavagga",
    "ai-thag2.21:0.4_0": "2.21. Uttarattheragāthā",
    "ai-thag2.21:1.1_0": "I’ve fully understood the aggregates;",
    "ai-thag2.21:1.2_0": "I’ve eradicated craving;",
    "ai-thag2.21:1.3_0": "I’ve developed the factors of awakening,",
    "ai-thag2.21:1.4_0": "and attained the ending of defilements.",
    "ai-thag2.21:2.1_0": "Having completely understood the aggregates,",
    "ai-thag2.21:2.2_0": "Having given up all India, they went to the net,",
    "ai-thag2.21:2.3_0": "Having developed the awakening factors,",
    "ai-thag2.21:2.4_0": "I’ll be quenched without defilements.”",
    "ai-thag2.21:2.5_0": "“The senior monk Uttara,"
}