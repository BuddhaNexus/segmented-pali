{
    "ai-ja314:0.1_0": "Jātaka",
    "ai-ja314:0.2_0": "Catukkanipāta",
    "ai-ja314:0.3_0": "Pucimandavagga",
    "ai-ja314:0.4_0": "4. Lohakumbhijātaka",
    "ai-ja314:1.1_0": "“One who is diligent and discerning,",
    "ai-ja314:1.2_0": "when you’re not tamed,",
    "ai-ja314:1.3_0": "When there are grounds for wealth,",
    "ai-ja314:1.4_0": "not having compassion for oneself.”",
    "ai-ja314:2.1_0": "“For sixty thousand years,",
    "ai-ja314:2.2_0": "complete, full;",
    "ai-ja314:2.3_0": "Reborn in hell,",
    "ai-ja314:2.4_0": "When will it be?”",
    "ai-ja314:3.1_0": "“There’s no way I’ll get angry,",
    "ai-ja314:3.2_0": "he won’t make a claim.",
    "ai-ja314:3.3_0": "But the bad things that do bring,",
    "ai-ja314:3.4_0": "which is yours, good sirs.”",
    "ai-ja314:4.1_0": "I went from here and there,",
    "ai-ja314:4.2_0": "I’ve gained the human state",
    "ai-ja314:4.3_0": "looks after the welfare of relatives—",
    "ai-ja314:4.4_0": "I’m skilled in what is skillful.”",
    "ai-ja314:5.1_0": "and fourth in pot."
}