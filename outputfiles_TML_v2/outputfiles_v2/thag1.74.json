    "ai-thag1.74:0.3_0": "Aṭṭhamavagga",
    "ai-thag1.74:0.4_0": "1.74. Suyāmanattheragāthā",
    "ai-thag1.74:1.1_0": "Sensual desire, ill will,",
    "ai-thag1.74:1.2_0": "dullness and drowsiness,",
