{
    "ai-thag1.105:0.1_0": "Theragāthā",
    "ai-thag1.105:0.2_0": "Ekakanipāta",
    "ai-thag1.105:0.3_0": "Ekādasamavagga",
    "ai-thag1.105:0.4_0": "Takkali",
    "ai-thag1.105:1.1_0": "Dissatisfied, one should not stay;",
    "ai-thag1.105:1.2_0": "and even if happy, one should depart.",
    "ai-thag1.105:1.3_0": "One who sees clearly wouldn’t stay",
    "ai-thag1.105:1.4_0": "for the trainee’s sake.”",
    "ai-thag1.105:1.5_0": "the senior venerable Macchikāsaṇḍa ..."
}