{
    "ai-ja163:0.1_0": "Jātaka",
    "ai-ja163:0.2_0": "Dukanipāta",
    "ai-ja163:0.3_0": "Santhavavagga",
    "ai-ja163:0.4_0": "3. Susīmajātaka",
    "ai-ja163:1.1_0": "“Who took the reeds you like a pyre,",
    "ai-ja163:1.2_0": "the winter enjoying the finest,",
    "ai-ja163:1.3_0": "you’ll give to you, Susīma,",
    "ai-ja163:1.4_0": "recollecting the ghost realm.”",
    "ai-ja163:2.1_0": "“Whether the deer in a corpse is like a beast,",
    "ai-ja163:2.2_0": "the winter enjoying the finest,",
    "ai-ja163:2.3_0": "they give it to you, student, I say.",
    "ai-ja163:2.4_0": "recollecting the ghost realm.”",
    "ai-ja163:3.1_0": "and third group of gods."
}