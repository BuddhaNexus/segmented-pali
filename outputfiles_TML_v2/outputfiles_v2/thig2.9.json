{
    "ai-thig2.9:0.1_0": "Therīgāthā",
    "ai-thig2.9:0.2_0": "Dukanipāta",
    "ai-thig2.9:0.3_0": "9. Abhayātherīgāthā",
    "ai-thig2.9:1.1_0": "Abhayā, the body is fragile,",
    "ai-thig2.9:1.2_0": "yet ordinary people are attached to it.",
    "ai-thig2.9:1.3_0": "I'll lay down the body,",
    "ai-thig2.9:1.4_0": "aware and mindful.",
    "ai-thig2.9:2.1_0": "Though subject to so many painful things,",
    "ai-thig2.9:2.2_0": "I have, through my love of diligence,",
    "ai-thig2.9:2.3_0": "reached the ending of craving,",
    "ai-thig2.9:2.4_0": "and fulfilled the Buddha’s instructions.",
    "ai-thig2.9:3.1_0": ", senior monks,"
}