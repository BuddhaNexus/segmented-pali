{
    "ai-thag2.38:0.1_0": "Theragāthā",
    "ai-thag2.38:0.2_0": "Dukanipāta",
    "ai-thag2.38:0.3_0": "Catutthavagga",
    "ai-thag2.38:0.4_0": "2.38. Nisabhattheragāthā",
    "ai-thag2.38:1.1_0": "I gave up the five kinds of sensual stimulation,",
    "ai-thag2.38:1.2_0": "so pleasing and delightful,",
    "ai-thag2.38:1.3_0": "and who’s left the home life out of faith—",
    "ai-thag2.38:1.4_0": "let them make an end to suffering!",
    "ai-thag2.38:2.1_0": "I don’t long for death;",
    "ai-thag2.38:2.2_0": "I don’t long for life;",
    "ai-thag2.38:2.3_0": "I await my time,",
    "ai-thag2.38:2.4_0": "aware and mindful.",
    "ai-thag2.38:2.5_0": "a senior monk ..."
}