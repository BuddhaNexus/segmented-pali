{
    "ai-thig5.10:0.1_0": "Therīgāthā",
    "ai-thig5.10:0.2_0": "Pañcakanipāta",
    "ai-thig5.10:0.3_0": "10. Paṭācārātherīgāthā",
    "ai-thig5.10:1.1_0": "Plowing the fields,",
    "ai-thig5.10:1.2_0": "the seeds have fallen.",
    "ai-thig5.10:1.3_0": "supporting partners and children,",
    "ai-thig5.10:1.4_0": "young men acquire wealth.",
    "ai-thig5.10:2.1_0": "I am accomplished in ethics,",
    "ai-thig5.10:2.2_0": "and I do the Teacher’s bidding,",
    "ai-thig5.10:2.3_0": "being neither lazy nor restless—",
    "ai-thig5.10:2.4_0": "why then do I not achieve quenching?",
    "ai-thig5.10:3.1_0": "they washed their feet",
    "ai-thig5.10:3.2_0": "make the water in the water,",
    "ai-thig5.10:3.3_0": "seeing the currents,",
    "ai-thig5.10:3.4_0": "flowing from high ground to low.",
    "ai-thig5.10:4.1_0": "My mind became serene,",
    "ai-thig5.10:4.2_0": "like a fine thoroughbred steed.",
    "ai-thig5.10:4.3_0": "Then, taking a lamp,",
    "ai-thig5.10:4.4_0": "I entered my dwelling,",
    "ai-thig5.10:4.5_0": "inspected the bed,",
    "ai-thig5.10:4.6_0": "I sat on a cot:",
    "ai-thig5.10:5.1_0": "Then the needles seized",
    "ai-thig5.10:5.2_0": "I drew out the wick.",
    "ai-thig5.10:5.3_0": "The liberation of my heart",
    "ai-thig5.10:5.4_0": "was like the quenching of the lamp.",
    "ai-thig5.10:6.1_0": "Paṭācārā ..."
}