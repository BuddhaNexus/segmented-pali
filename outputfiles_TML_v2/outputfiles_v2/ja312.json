{
    "ai-ja312:0.1_0": "Jātaka",
    "ai-ja312:0.2_0": "Catukkanipāta",
    "ai-ja312:0.3_0": "Pucimandavagga",
    "ai-ja312:0.4_0": "2. Kassapamandiyajātaka",
    "ai-ja312:1.1_0": "“Kassapa, the Nerañjara.",
    "ai-ja312:1.2_0": "sweeps away the wind,",
    "ai-ja312:1.3_0": "all that wise one is bearing for,",
    "ai-ja312:1.4_0": "that astute person endures.",
    "ai-ja312:2.1_0": "Even if good people quarrel,",
    "ai-ja312:2.2_0": "quickly joints again and again,",
    "ai-ja312:2.3_0": "foolish, stupid people.",
    "ai-ja312:2.4_0": "don’t come to the far shore!",
    "ai-ja312:3.1_0": "These run here and now",
    "ai-ja312:3.2_0": "not jump from them,",
    "ai-ja312:3.3_0": "He who understands the generosity",
    "ai-ja312:3.4_0": "But one who understands the teaching of the Dhamma,",
    "ai-ja312:4.1_0": "There is no better than him,",
    "ai-ja312:4.2_0": "Remembering the burden,",
    "ai-ja312:4.3_0": "The one who understands others’ blame",
    "ai-ja312:4.4_0": "I remember myself.”",
    "ai-ja312:5.1_0": "Kassapa had the second wonder,"
}