{
    "ai-tha-ap190:0.1_0": "Therāpadāna",
    "ai-tha-ap190:0.2_0": "Kuṭajapupphiyavagga",
    "ai-tha-ap190:0.3_0": "The Elder Tissa related the Flame Tree",
    "ai-tha-ap190:1.1_0": "“I’m a peaceful one,",
    "ai-tha-ap190:1.2_0": "honored by the nymphs.",
    "ai-tha-ap190:1.3_0": "After running on the past,",
    "ai-tha-ap190:1.4_0": "recollecting the Buddha,",
    "ai-tha-ap190:2.1_0": "Tissa the lotuses on the bank of the river,",
    "ai-tha-ap190:2.2_0": "my mind confidently settled,",
    "ai-tha-ap190:2.3_0": "I crushed the Buddha.",
    "ai-tha-ap190:2.4_0": "I’m not even fortunate,",
    "ai-tha-ap190:3.1_0": "A single eon irrevers:",
    "ai-tha-ap190:3.2_0": "As a bird was worshipped by the flowers,",
    "ai-tha-ap190:3.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap190:3.4_0": "this fruit is to know,",
    "ai-tha-ap190:4.1_0": "and seven eons have been stored up,",
    "ai-tha-ap190:4.2_0": "Four months...",
    "ai-tha-ap190:4.3_0": "Endowed with the seven gems,",
    "ai-tha-ap190:4.4_0": "The Many-Carried wheels",
    "ai-tha-ap190:5.1_0": "the four analytical knowledges,",
    "ai-tha-ap190:5.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap190:5.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap190:6.1_0": "That is how these verses were recited by the senior venerable Tikaṇṇa.",
    "ai-tha-ap190:7.1_0": "The eighth Elder Tikaṇṇika"
}