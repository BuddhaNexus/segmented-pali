{
    "ai-ja51:0.1_0": "Jātaka",
    "ai-ja51:0.2_0": "Ekakanipāta",
    "ai-ja51:0.3_0": "Āsīsavagga",
    "ai-ja51:0.4_0": "1. Mahāsīlavajātaka",
    "ai-ja51:1.1_0": "“A man should sleep at leaf-hut,",
    "ai-ja51:1.2_0": "should never grow disillusioned.",
    "ai-ja51:1.3_0": "I see myself,",
    "ai-ja51:1.4_0": "as I wished.”",
    "ai-ja51:2.1_0": "The first kind of great virtue"
}