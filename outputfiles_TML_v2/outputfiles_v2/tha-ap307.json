{
    "ai-tha-ap307:0.1_0": "Therāpadāna",
    "ai-tha-ap307:0.2_0": "Padumakesaravagga",
    "ai-tha-ap307:0.3_0": "5. Phaladāyakattheraapadāna",
    "ai-tha-ap307:1.1_0": "“One from sharing.",
    "ai-tha-ap307:1.2_0": "it was this hermitage.",
    "ai-tha-ap307:1.3_0": "I shall go to that hermitage.",
    "ai-tha-ap307:1.4_0": "Leaving my hands, Etc.",
    "ai-tha-ap307:2.1_0": "I saw her.",
    "ai-tha-ap307:2.2_0": "I was drunken on the rainy season.",
    "ai-tha-ap307:2.3_0": "whether there is fruit,",
    "ai-tha-ap307:2.4_0": "I practise a sword,",
    "ai-tha-ap307:3.1_0": "A single eon irrevers:",
    "ai-tha-ap307:3.2_0": "the fruitless (of rebirth).",
    "ai-tha-ap307:3.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap307:3.4_0": "as fruit is the fruit.",
    "ai-tha-ap307:4.1_0": "the four analytical knowledges,",
    "ai-tha-ap307:4.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap307:4.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap307:5.1_0": "That is how these verses were recited by the senior venerable Soṇa.",
    "ai-tha-ap307:6.1_0": "for the fifth Elder Nun Pūraṇa Kassapa"
}