{
    "ai-ja155:0.1_0": "Jātaka",
    "ai-ja155:0.2_0": "Dukanipāta",
    "ai-ja155:0.3_0": "Daḷhavagga",
    "ai-ja155:0.4_0": "5. Bhaggajātaka",
    "ai-ja155:1.1_0": "“Life a hundred years, Bhaggava,",
    "ai-ja155:1.2_0": "Another twenty years, twenty,",
    "ai-ja155:1.3_0": "may eat meat and milk-rice.",
    "ai-ja155:1.4_0": "You live mindfully.”",
    "ai-ja155:2.1_0": "“Even if you live for a hundred years,",
    "ai-ja155:2.2_0": "Another twenty years, twenty,",
    "ai-ja155:2.3_0": "he eats poison,",
    "ai-ja155:2.4_0": "You live mindfully.”",
    "ai-ja155:3.1_0": "The fifth sub-chapter on lying down"
}