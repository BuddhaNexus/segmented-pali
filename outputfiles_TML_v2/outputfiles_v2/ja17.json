{
    "ai-ja17:0.1_0": "Jātaka",
    "ai-ja17:0.2_0": "Ekakanipāta",
    "ai-ja17:0.3_0": "Sīlavagga",
    "ai-ja17:0.4_0": "7. Mālutajātaka",
    "ai-ja17:1.1_0": "“Whether black or white,",
    "ai-ja17:1.2_0": "As with the wind:",
    "ai-ja17:1.3_0": "For the cool water for cool water—",
    "ai-ja17:1.4_0": "both of these are subtle.”",
    "ai-ja17:2.1_0": "like the seventh maker."
}