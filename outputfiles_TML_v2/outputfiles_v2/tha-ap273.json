{
    "ai-tha-ap273:0.1_0": "Therāpadāna",
    "ai-tha-ap273:0.2_0": "Suvaṇṇabibbohanavagga",
    "ai-tha-ap273:0.3_0": "The Elders then having a golden plate on the ground,",
    "ai-tha-ap273:1.1_0": "“A giver of food,",
    "ai-tha-ap273:1.2_0": "confident in the crooks,",
    "ai-tha-ap273:1.3_0": "I took a keyhole,",
    "ai-tha-ap273:1.4_0": "as proper attention.",
    "ai-tha-ap273:2.1_0": "A single eon irrevers:",
    "ai-tha-ap273:2.2_0": "I’ll defeat the arm!",
    "ai-tha-ap273:2.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap273:2.4_0": "this is the (Path.) fruit,",
    "ai-tha-ap273:3.1_0": "You have these eighteen eons",
    "ai-tha-ap273:3.2_0": "An aristocrat:",
    "ai-tha-ap273:3.3_0": "endowed with the seven treasures,",
    "ai-tha-ap273:3.4_0": "The Mahallika conversions",
    "ai-tha-ap273:4.1_0": "the four analytical knowledges,",
    "ai-tha-ap273:4.2_0": "having excellence and liberation,",
    "ai-tha-ap273:4.3_0": "I’ve realized the six kinds of direct knowledge,",
    "ai-tha-ap273:4.4_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap273:5.1_0": "That is how these verses were recited by the senior venerable Posture.",
    "ai-tha-ap273:6.1_0": "he first (gods) from the Elder top of the Elders,"
}