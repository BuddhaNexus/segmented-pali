{
    "ai-ja153:0.1_0": "Jātaka",
    "ai-ja153:0.2_0": "Dukanipāta",
    "ai-ja153:0.3_0": "Daḷhavagga",
    "ai-ja153:0.4_0": "3. Sūkarajātaka",
    "ai-ja153:1.1_0": "“Seeing the four quarters, dear Saṁsāra,",
    "ai-ja153:1.2_0": "You also have four feet,",
    "ai-ja153:1.3_0": "Come, my good man,",
    "ai-ja153:1.4_0": "Why are you afraid of?”",
    "ai-ja153:2.1_0": "“Susīma, Pūlaka.",
    "ai-ja153:2.2_0": "so hard to defeat, pigs’ snare.",
    "ai-ja153:2.3_0": "“If you want to make you out,",
    "ai-ja153:2.4_0": "I give you any victory.”",
    "ai-ja153:3.1_0": "The third group of nine is finished."
}