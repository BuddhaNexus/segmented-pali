{
    "ai-pv29:0.1_0": "Petavatthu",
    "ai-pv29:0.2_0": "Cūḷavagga",
    "ai-pv29:0.3_0": "4. Bhusapetavatthu",
    "ai-pv29:1.1_0": "“Sāli, one is envious;",
    "ai-pv29:1.2_0": "That shade of foes domain of pleasure.",
    "ai-pv29:1.3_0": "And you’ll filthy, stinking,",
    "ai-pv29:1.4_0": "This result will you be?”",
    "ai-pv29:2.1_0": "“In the past, future, or present,",
    "ai-pv29:2.2_0": "But this fraudulent calf is not small,",
    "ai-pv29:2.3_0": "This meat eats up with the flesh.",
    "ai-pv29:2.4_0": "If you lie down with false speech:",
    "ai-pv29:3.1_0": "A human beings,",
    "ai-pv29:3.2_0": "went to the hermit’s house.",
    "ai-pv29:3.3_0": "We’ve swept away the peaceful ones;",
    "ai-pv29:3.4_0": "Don’t let anything bad!",
    "ai-pv29:4.1_0": "I’m practicing right.",
    "ai-pv29:4.2_0": "‘There’s no home for me,’",
    "ai-pv29:4.3_0": "If you call out, swept away,",
    "ai-pv29:4.4_0": "let me eat my food.’",
    "ai-pv29:5.1_0": "result of deeds,",
    "ai-pv29:5.2_0": "and the dangers for lying;",
    "ai-pv29:5.3_0": "Almsfood, rice,",
    "ai-pv29:5.4_0": "he swaps me a dung.",
    "ai-pv29:6.1_0": "a barren worker,",
    "ai-pv29:6.2_0": "But you won’t perish—",
    "ai-pv29:6.3_0": "What’s the point of foulness?",
    "ai-pv29:6.4_0": "and I eat.”",
    "ai-pv29:7.1_0": "The fourth ground for human beings."
}