{
    "ai-tha-ap127:0.1_0": "Therāpadāna",
    "ai-tha-ap127:0.2_0": "Sereyyavagga",
    "ai-tha-ap127:0.3_0": "The Elder Tissa Majesty",
    "ai-tha-ap127:1.1_0": "“I’m a scholar",
    "ai-tha-ap127:1.2_0": "I answered the signs",
    "ai-tha-ap127:1.3_0": "‘Having nothing is given to people,",
    "ai-tha-ap127:1.4_0": "In the world he will be the Buddha.",
    "ai-tha-ap127:2.1_0": "Yet I grieve for a long time,",
    "ai-tha-ap127:2.2_0": "Ten thousand people.",
    "ai-tha-ap127:2.3_0": "now is the Blessed One,",
    "ai-tha-ap127:2.4_0": "the Seer teaches Dhamma,",
    "ai-tha-ap127:3.1_0": "Yet I grieve for a long time,",
    "ai-tha-ap127:3.2_0": "discerning, unclouded,",
    "ai-tha-ap127:3.3_0": "now is the Blessed One,",
    "ai-tha-ap127:3.4_0": "the Seer teaches Dhamma,",
    "ai-tha-ap127:4.1_0": "Yet I grieve for a long time,",
    "ai-tha-ap127:4.2_0": "flowing along the rivers.",
    "ai-tha-ap127:4.3_0": "now is the Blessed One,",
    "ai-tha-ap127:4.4_0": "the Seer teaches Dhamma,",
    "ai-tha-ap127:5.1_0": "Yet I grieve for a long time,",
    "ai-tha-ap127:5.2_0": "without forty purging the fire.",
    "ai-tha-ap127:5.3_0": "now is the Blessed One,",
    "ai-tha-ap127:5.4_0": "the Seer teaches Dhamma,",
    "ai-tha-ap127:6.1_0": "Yet I grieve for a long time,",
    "ai-tha-ap127:6.2_0": "Leaving my limbs, and live on,",
    "ai-tha-ap127:6.3_0": "now is the Blessed One,",
    "ai-tha-ap127:6.4_0": "the Seer teaches Dhamma,",
    "ai-tha-ap127:7.1_0": "Yet I grieve for a long time,",
    "ai-tha-ap127:7.2_0": "the wind doesn’t constituent.",
    "ai-tha-ap127:7.3_0": "now is the Blessed One,",
    "ai-tha-ap127:7.4_0": "the Seer teaches Dhamma,",
    "ai-tha-ap127:8.1_0": "Yet I grieve for a long time,",
    "ai-tha-ap127:8.2_0": "all precious things:",
    "ai-tha-ap127:8.3_0": "now is the Blessed One,",
    "ai-tha-ap127:8.4_0": "the Seer teaches Dhamma,",
    "ai-tha-ap127:9.1_0": "Yet I grieve for a long time,",
    "ai-tha-ap127:9.2_0": "Seven on foot,",
    "ai-tha-ap127:9.3_0": "now is the Blessed One,",
    "ai-tha-ap127:9.4_0": "the Seer teaches Dhamma,",
    "ai-tha-ap127:10.1_0": "the Buddha is born,",
    "ai-tha-ap127:10.2_0": "he saw all quarters.",
    "ai-tha-ap127:10.3_0": "on your forehead.",
    "ai-tha-ap127:10.4_0": "this is the teaching.’",
    "ai-tha-ap127:11.1_0": "having grasped the people,",
    "ai-tha-ap127:11.2_0": "smooth the world!",
    "ai-tha-ap127:11.3_0": "worshipped the Buddha,",
    "ai-tha-ap127:11.4_0": "went away from Pāvā to see her.",
    "ai-tha-ap127:12.1_0": "A single eon irrevers:",
    "ai-tha-ap127:12.2_0": "the Buddha, who worships the Buddha,",
    "ai-tha-ap127:12.3_0": "I’m not aware of any living being",
    "ai-tha-ap127:12.4_0": "This is the fruit,",
    "ai-tha-ap127:13.1_0": "weeping under here and now;",
    "ai-tha-ap127:13.2_0": "it has been resolved face-to-face.",
    "ai-tha-ap127:13.3_0": "endowed with the seven treasures,",
    "ai-tha-ap127:13.4_0": "The Mahallika conversions",
    "ai-tha-ap127:14.1_0": "The earth:",
    "ai-tha-ap127:14.2_0": "staying alone in one place.",
    "ai-tha-ap127:14.3_0": "endowed with the seven treasures,",
    "ai-tha-ap127:14.4_0": "The Mahallika conversions",
    "ai-tha-ap127:15.1_0": "and eighteen eons.",
    "ai-tha-ap127:15.2_0": "This is called “ascetic”.",
    "ai-tha-ap127:15.3_0": "endowed with the seven treasures,",
    "ai-tha-ap127:15.4_0": "The Mahallika conversions",
    "ai-tha-ap127:16.1_0": "and poised seven days.",
    "ai-tha-ap127:16.2_0": "Forms Bounded",
    "ai-tha-ap127:16.3_0": "endowed with the seven treasures,",
    "ai-tha-ap127:16.4_0": "The Mahallika conversions",
    "ai-tha-ap127:17.1_0": "A fire:",
    "ai-tha-ap127:17.2_0": "of the sixteen eons.",
    "ai-tha-ap127:17.3_0": "endowed with the seven treasures,",
    "ai-tha-ap127:17.4_0": "The Mahallika conversions",
    "ai-tha-ap127:18.1_0": "Fetters:",
    "ai-tha-ap127:18.2_0": "for five reasons.",
    "ai-tha-ap127:18.3_0": "endowed with the seven treasures,",
    "ai-tha-ap127:18.4_0": "The Mahallika conversions",
    "ai-tha-ap127:19.1_0": "A ruler:",
    "ai-tha-ap127:19.2_0": "and she became well-known as a bowl.",
    "ai-tha-ap127:19.3_0": "endowed with the seven treasures,",
    "ai-tha-ap127:19.4_0": "The Mahallika conversions",
    "ai-tha-ap127:20.1_0": "A gem:",
    "ai-tha-ap127:20.2_0": "of the one eons.",
    "ai-tha-ap127:20.3_0": "endowed with the seven treasures,",
    "ai-tha-ap127:20.4_0": "The Mahallika conversions",
    "ai-tha-ap127:21.1_0": "Strapped by the strike:",
    "ai-tha-ap127:21.2_0": "Seeing the two eons",
    "ai-tha-ap127:21.3_0": "endowed with the seven treasures,",
    "ai-tha-ap127:21.4_0": "The Mahallika conversions",
    "ai-tha-ap127:22.1_0": "A king:",
    "ai-tha-ap127:22.2_0": "if it is allowable;",
    "ai-tha-ap127:22.3_0": "endowed with the seven treasures,",
    "ai-tha-ap127:22.4_0": "The Mahallika conversions",
    "ai-tha-ap127:23.1_0": "“Regarding a word of the world,",
    "ai-tha-ap127:23.2_0": "I am an aristocrat,",
    "ai-tha-ap127:23.3_0": "endowed with the seven treasures,",
    "ai-tha-ap127:23.4_0": "The Mahallika conversions",
    "ai-tha-ap127:24.1_0": "the four analytical knowledges,",
    "ai-tha-ap127:24.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap127:24.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap127:25.1_0": "That is how these verses were recited in the presence of the senior venerable Saṅgāmajī.",
    "ai-tha-ap127:26.1_0": "The fifth Elder Suttanta."
}