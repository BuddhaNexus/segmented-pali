{
    "ai-tha-ap509:0.1_0": "Therāpadāna",
    "ai-tha-ap509:0.2_0": "Kaṇikāravagga",
    "ai-tha-ap509:0.3_0": "° he gave the gift of the Elder Nun Piṭaka Rohaṇa",
    "ai-tha-ap509:1.1_0": "“Awakened today, Awakened One,",
    "ai-tha-ap509:1.2_0": "like a swift stream in the Himālaya.",
    "ai-tha-ap509:1.3_0": "Endowed with good conduct,",
    "ai-tha-ap509:1.4_0": "a sage, skilled in immersion,",
    "ai-tha-ap509:2.1_0": "Having taken a potter",
    "ai-tha-ap509:2.2_0": "I lived alive,",
    "ai-tha-ap509:2.3_0": "After taking a _kahāpaṇa_ coin,",
    "ai-tha-ap509:2.4_0": "I practise a sword,",
    "ai-tha-ap509:3.1_0": "if he makes his four eons;",
    "ai-tha-ap509:3.2_0": "the fruitless (of rebirth).",
    "ai-tha-ap509:3.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap509:3.4_0": "as fruit is the fruit.",
    "ai-tha-ap509:4.1_0": "corrupted by defilements,",
    "ai-tha-ap509:4.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap509:4.3_0": "and live without defilements.",
    "ai-tha-ap509:5.1_0": "It was so welcome for me",
    "ai-tha-ap509:5.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap509:5.3_0": "and fulfilled the Buddha’s instructions.",
    "ai-tha-ap509:6.1_0": "the four analytical knowledges,",
    "ai-tha-ap509:6.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap509:6.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap509:7.1_0": "That is how these verses were recited by the senior venerable Ajita.",
    "ai-tha-ap509:8.1_0": "after attaining the seventh fruit of that very fruitful,"
}