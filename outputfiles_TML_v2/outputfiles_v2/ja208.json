{
    "ai-ja208:0.1_0": "Jātaka",
    "ai-ja208:0.2_0": "Dukanipāta",
    "ai-ja208:0.3_0": "Nataṁdaḷhavagga",
    "ai-ja208:0.4_0": "8. Susumārajātaka",
    "ai-ja208:1.1_0": "“Enough, my mangoes.",
    "ai-ja208:1.2_0": "Where is the Rose-Apple Island",
    "ai-ja208:1.3_0": "the far shore,",
    "ai-ja208:1.4_0": "It’s better to my stomach-pulled up like fibre.”",
    "ai-ja208:2.1_0": "it’s so very hard to find—",
    "ai-ja208:2.2_0": "nor do they adversity,",
    "ai-ja208:2.3_0": "You pretty judges.",
    "ai-ja208:2.4_0": "Go to the happiness of the people.”",
    "ai-ja208:3.1_0": "Eighth was the eighth day."
}