{
    "ai-thag1.23:0.1_0": "Theragāthā",
    "ai-thag1.23:0.2_0": "Ekakanipāta",
    "ai-thag1.23:0.3_0": "Tatiyavagga",
    "ai-thag1.23:0.4_0": "1.23. Gosālattheragāthā",
    "ai-thag1.23:1.1_0": "I’ll eat honey and milk-rice",
    "ai-thag1.23:1.2_0": "in Veḷugumba.",
    "ai-thag1.23:1.3_0": "after circumambulating him,",
    "ai-thag1.23:1.4_0": "of phenomena such as the aggregates.",
    "ai-thag1.23:1.5_0": "I’ll set up a regular monk",
    "ai-thag1.23:1.6_0": "and foster seclusion.",
    "ai-thag1.23:1.7_0": "... Gosī ..."
}