{
    "ai-ja243:0.1_0": "Jātaka",
    "ai-ja243:0.2_0": "Dukanipāta",
    "ai-ja243:0.3_0": "Siṅgālavagga",
    "ai-ja243:0.4_0": "3. Guttilajātaka",
    "ai-ja243:1.1_0": "“We call a cow",
    "ai-ja243:1.2_0": "made the delightful Universal.",
    "ai-ja243:1.3_0": "He blocks me in the position,",
    "ai-ja243:1.4_0": "to take refuge in me.”",
    "ai-ja243:2.1_0": "“By this I have gone for refuge to you,",
    "ai-ja243:2.2_0": "caring for ignorant and terrified,",
    "ai-ja243:2.3_0": "there will be no conquering,",
    "ai-ja243:2.4_0": "you’ll live mindfully.”",
    "ai-ja243:3.1_0": "Secluded in her duty,"
}