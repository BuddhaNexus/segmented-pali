{
    "ai-thag1.47:0.1_0": "Theragāthā",
    "ai-thag1.47:0.2_0": "Ekakanipāta",
    "ai-thag1.47:0.3_0": "Pañcamavagga",
    "ai-thag1.47:0.4_0": "1.47. Ujjayattheragāthā",
    "ai-thag1.47:1.1_0": "Homage to you, O Buddha, O hero,",
    "ai-thag1.47:1.2_0": "freed in every way!",
    "ai-thag1.47:1.3_0": "Meditating in the fruits of your practice,",
    "ai-thag1.47:1.4_0": "I live without defilements.",
    "ai-thag1.47:1.5_0": "... The senior monk Khujjasobhita ..."
}