{
    "ai-vv51:0.1_0": "Vimānavatthu",
    "ai-vv51:0.2_0": "Purisavimāna",
    "ai-vv51:0.3_0": "Mahārathavagga",
    "ai-vv51:0.4_0": "The Simile of the Man",
    "ai-vv51:1.1_0": "“Who now bows and feet,",
    "ai-vv51:1.2_0": "shining with power and glory.",
    "ai-vv51:1.3_0": "Excellent, beautiful,",
    "ai-vv51:1.4_0": "lighting up the entire direction.”",
    "ai-vv51:2.1_0": "“I used to make a piece of meat,",
    "ai-vv51:2.2_0": "whose habitat is the water.",
    "ai-vv51:2.3_0": "listen to the teaching,",
    "ai-vv51:2.4_0": "the one with pebbles the Vacchapāla",
    "ai-vv51:3.1_0": "confident heart,",
    "ai-vv51:3.2_0": "I have psychic power, fame,",
    "ai-vv51:3.3_0": "confident in my psychic power,",
    "ai-vv51:3.4_0": "see my beauty and happiness.",
    "ai-vv51:4.1_0": "And those who have a long time been for those who have a long time,",
    "ai-vv51:4.2_0": "They heard the teaching,",
    "ai-vv51:4.3_0": "having found unshakable water.",
    "ai-vv51:4.4_0": "where there is no sorrow.”",
    "ai-vv51:5.1_0": "The gods of the Thirty-Three"
}