{
    "ai-ja209:0.1_0": "Jātaka",
    "ai-ja209:0.2_0": "Dukanipāta",
    "ai-ja209:0.3_0": "Nataṁdaḷhavagga",
    "ai-ja209:0.4_0": "9. Kukkuṭajātaka",
    "ai-ja209:1.1_0": "I’ve seen a tree in the jungle,",
    "ai-ja209:1.2_0": "tail-covered heads,",
    "ai-ja209:1.3_0": "But you don’t think,",
    "ai-ja209:1.4_0": "Like that you, Sakyan.”",
    "ai-ja209:2.1_0": "“This chicken’s feathers is a chicken’s eggs,",
    "ai-ja209:2.2_0": "afraid, the tortoise,",
    "ai-ja209:2.3_0": "A skillful person",
    "ai-ja209:2.4_0": "which departs without first expectation.”",
    "ai-ja209:3.1_0": "chickens and pigs,"
}