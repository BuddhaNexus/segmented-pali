{
    "ai-ja10:0.1_0": "Jātaka",
    "ai-ja10:0.2_0": "Ekakanipāta",
    "ai-ja10:0.3_0": "Apaṇṇakavagga",
    "ai-ja10:0.4_0": "10. Sukhavihārijātaka",
    "ai-ja10:1.1_0": "“Neither with the good,",
    "ai-ja10:1.2_0": "nor protecting others:",
    "ai-ja10:1.3_0": "such a one sleeps at ease,",
    "ai-ja10:1.4_0": "unconcerned for sensual pleasures?”",
    "ai-ja10:2.1_0": "a happy abiding in bliss.",
    "ai-ja10:2.2_0": "The first sub-chapter on lying down is finished.",
    "ai-ja10:3.1_0": "This is the summary:",
    "ai-ja10:4.1_0": "like a golden statue, like the sun.",
    "ai-ja10:4.2_0": "festivals and bagskets,",
    "ai-ja10:4.3_0": "Consumed by children and partners,",
    "ai-ja10:4.4_0": "But if you don’t protect them,"
}