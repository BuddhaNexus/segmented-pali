{
    "ai-thig3.6:0.1_0": "Therīgāthā",
    "ai-thig3.6:0.2_0": "Tikanipāta",
    "ai-thig3.6:0.3_0": "6. Sukkātherīgāthā",
    "ai-thig3.6:1.1_0": "“What’s up with these people in Rājagaha?",
    "ai-thig3.6:1.2_0": "They sprawl like they’ve been drinking mead!",
    "ai-thig3.6:1.3_0": "They don’t attend on Sukkā",
    "ai-thig3.6:1.4_0": "as she teaches the Buddha’s instructions.",
    "ai-thig3.6:2.1_0": "But the wise—",
    "ai-thig3.6:2.2_0": "so irresistible, delicious and nutritious.",
    "ai-thig3.6:2.3_0": "so irresistible, delicious and nutritious,",
    "ai-thig3.6:2.4_0": "like travelers enjoying a cool cloud.”",
    "ai-thig3.6:3.1_0": "S bright qualities—",
    "ai-thig3.6:3.2_0": "free of greed, serene.",
    "ai-thig3.6:3.3_0": "He bears his final body,",
    "ai-thig3.6:3.4_0": "bear your final body.",
    "ai-thig3.6:4.1_0": "Sukkā,"
}