{
    "ai-thag1.66:0.1_0": "Theragāthā",
    "ai-thag1.66:0.2_0": "Ekakanipāta",
    "ai-thag1.66:0.3_0": "Sattamavagga",
    "ai-thag1.66:0.4_0": "1.66. Meghiyattheragāthā",
    "ai-thag1.66:1.1_0": "He counseled me, the great hero,",
    "ai-thag1.66:1.2_0": "gone beyond all things,",
    "ai-thag1.66:1.3_0": "When I heard his teaching",
    "ai-thag1.66:1.4_0": "I stayed close by him, mindful.",
    "ai-thag1.66:1.5_0": "I’ve attained the three knowledges;",
    "ai-thag1.66:1.6_0": "and fulfilled the Buddha’s instructions.",
    "ai-thag1.66:1.7_0": "Meghiya,"
}