{
    "ai-thag1.70:0.1_0": "Theragāthā",
    "ai-thag1.70:0.2_0": "Ekakanipāta",
    "ai-thag1.70:0.3_0": "Sattamavagga",
    "ai-thag1.70:0.4_0": "1.70. Puṇṇattheragāthā",
    "ai-thag1.70:1.1_0": "Ethical conduct is best in this life,",
    "ai-thag1.70:1.2_0": "but one with wisdom is supreme.",
    "ai-thag1.70:1.3_0": "Someone with both virtue and wisdom",
    "ai-thag1.70:1.4_0": "ethics and wisdom.”",
    "ai-thag1.70:1.5_0": "…",
    "ai-thag1.70:1.6_0": "a senior monk,",
    "ai-thag1.70:1.7_0": "The seventh sub-chapter on lying down is finished.",
    "ai-thag1.70:2.0_0": "“This is the summary:",
    "ai-thag1.70:2.1_0": "“Vajjis, Sajjis,",
    "ai-thag1.70:2.2_0": "immaculate, stainless,",
    "ai-thag1.70:2.3_0": "driven off the grains,",
    "ai-thag1.70:2.4_0": "Megharājas.",
    "ai-thag1.70:2.5_0": "they seek just one partner",
    "ai-thag1.70:2.6_0": "The Elder Mahalla, Abhaya,"
}