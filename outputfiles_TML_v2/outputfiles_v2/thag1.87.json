{
    "ai-thag1.87:0.1_0": "Theragāthā",
    "ai-thag1.87:0.2_0": "Ekakanipāta",
    "ai-thag1.87:0.3_0": "Navamavagga",
    "ai-thag1.87:0.4_0": "1.87. Paviṭṭhattheragāthā",
    "ai-thag1.87:1.1_0": "The aggregates are seen as they truly are;",
    "ai-thag1.87:1.2_0": "all rebirths are shattered;",
    "ai-thag1.87:1.3_0": "transmigration through births is finished;",
    "ai-thag1.87:1.4_0": "now there’ll be no more future lives.”",
    "ai-thag1.87:1.5_0": "the senior monk,"
}