{
    "ai-thag2.20:0.1_0": "Theragāthā",
    "ai-thag2.20:0.2_0": "Dukanipāta",
    "ai-thag2.20:0.3_0": "Dutiyavagga",
    "ai-thag2.20:0.4_0": "2.20. Sirimattheragāthā",
    "ai-thag2.20:1.1_0": "If others praise one",
    "ai-thag2.20:1.2_0": "who has no immersion,",
    "ai-thag2.20:1.3_0": "they praise in vain,",
    "ai-thag2.20:1.4_0": "as one has no immersion.",
    "ai-thag2.20:2.1_0": "If others rebuke one",
    "ai-thag2.20:2.2_0": "who does have immersion,",
    "ai-thag2.20:2.3_0": "they rebuke in vain,",
    "ai-thag2.20:2.4_0": "as one does have immersion.",
    "ai-thag2.20:2.5_0": "…",
    "ai-thag2.20:2.6_0": "Sirs,",
    "ai-thag2.20:2.7_0": "The second sub-chapter on lying down is finished.",
    "ai-thag2.20:3.0_0": "“This is the summary:",
    "ai-thag2.20:3.1_0": "Cunda, Jotipāla, and Gandhamādana,",
    "ai-thag2.20:3.2_0": "Heraññakāni",
    "ai-thag2.20:3.3_0": "I’m shaken by all,",
    "ai-thag2.20:3.4_0": "I was Kimbila, Tissa,",
    "ai-thag2.20:3.5_0": "My head, and your feet,",
    "ai-thag2.20:3.6_0": "Ten seniors:"
}