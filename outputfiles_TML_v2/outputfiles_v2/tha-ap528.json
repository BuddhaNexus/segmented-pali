{
    "ai-tha-ap528:0.1_0": "Therāpadāna",
    "ai-tha-ap528:0.2_0": "Tiṇadāyakavagga",
    "ai-tha-ap528:0.3_0": "6. Daṇḍadāyakattheraapadāna",
    "ai-tha-ap528:1.1_0": "“You’re running the forest,",
    "ai-tha-ap528:1.2_0": "I’ve cut off the bamboo work.",
    "ai-tha-ap528:1.3_0": "make a wish,",
    "ai-tha-ap528:1.4_0": "I gave gifts to the Sangha.",
    "ai-tha-ap528:2.1_0": "with faithful mind,",
    "ai-tha-ap528:2.2_0": "you have been good enough",
    "ai-tha-ap528:2.3_0": "gave the mango grove,",
    "ai-tha-ap528:2.4_0": "went away.",
    "ai-tha-ap528:3.1_0": "if he makes his four eons;",
    "ai-tha-ap528:3.2_0": "I gave a gift to a stick",
    "ai-tha-ap528:3.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap528:3.4_0": "the gift of a staff.",
    "ai-tha-ap528:4.1_0": "corrupted by defilements,",
    "ai-tha-ap528:4.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap528:4.3_0": "and live without defilements.",
    "ai-tha-ap528:5.1_0": "It was so welcome for me",
    "ai-tha-ap528:5.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap528:5.3_0": "and fulfilled the Buddha’s instructions.",
    "ai-tha-ap528:6.1_0": "the four analytical knowledges,",
    "ai-tha-ap528:6.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap528:6.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap528:7.1_0": "That is how these verses were recited by the senior venerable Bhaddiya.",
    "ai-tha-ap528:8.1_0": "The sixth wives of sticks",
    "ai-tha-ap528:8.2_0": "at the twenty-two brothers."
}