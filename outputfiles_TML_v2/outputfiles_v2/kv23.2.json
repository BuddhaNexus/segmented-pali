{
    "ai-kv23.2:0.1_0": "Kathāvatthu",
    "ai-kv23.2:0.2_0": "Mahāpaṇṇāsaka",
    "ai-kv23.2:0.3_0": "Tevīsatimavagga",
    "ai-kv23.2:0.4_0": "Arahantavaṇṇakathā",
    "ai-kv23.2:1.1_0": "and use alcoholic drinks that cause negligence.",
    "ai-kv23.2:1.2_0": "Wemmoned out",
    "ai-kv23.2:1.3_0": "They spend their hands,",
    "ai-kv23.2:1.4_0": "speaking falsely,",
    "ai-kv23.2:1.5_0": "lies, he knows he is lying;",
    "ai-kv23.2:1.6_0": "who speak divisively,",
    "ai-kv23.2:1.7_0": "harsh speech;",
    "ai-kv23.2:1.8_0": "is worshipped by a cow.",
    "ai-kv23.2:1.9_0": "cutting down the rope.",
    "ai-kv23.2:1.10_0": "They collected sticks,",
    "ai-kv23.2:1.11_0": "They build a hut out of one sitting.",
    "ai-kv23.2:1.12_0": "Supported:",
    "ai-kv23.2:1.13_0": "they go to another’s house.",
    "ai-kv23.2:1.14_0": "ornamental hanging villages ...",
    "ai-kv23.2:1.15_0": "They build a town:",
    "ai-kv23.2:1.16_0": "And you don’t keep on doing it.",
    "ai-kv23.2:1.17_0": "The account of the perfected ones is finished."
}