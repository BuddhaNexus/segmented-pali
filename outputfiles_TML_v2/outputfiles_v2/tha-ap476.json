{
    "ai-tha-ap476:0.1_0": "Therāpadāna",
    "ai-tha-ap476:0.2_0": "Naḷamālivagga",
    "ai-tha-ap476:0.3_0": "he gave the Elder Suppiya",
    "ai-tha-ap476:1.1_0": "“Look at the Fortunate One’s Tapassī,",
    "ai-tha-ap476:1.2_0": "the Buddha-to-be,",
    "ai-tha-ap476:1.3_0": "Sumana, who had taken a seed,",
    "ai-tha-ap476:1.4_0": "I drunk the seeds to the supreme Buddha.",
    "ai-tha-ap476:2.1_0": "A single eon irrevers:",
    "ai-tha-ap476:2.2_0": "I drunk the seeds to the supreme Buddha.",
    "ai-tha-ap476:2.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap476:2.4_0": "this is the seed.",
    "ai-tha-ap476:3.1_0": "corrupted by defilements,",
    "ai-tha-ap476:3.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap476:3.3_0": "and live without defilements.",
    "ai-tha-ap476:4.1_0": "It was so welcome for me",
    "ai-tha-ap476:4.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap476:4.3_0": "and fulfilled the Buddha’s instructions.",
    "ai-tha-ap476:5.1_0": "the four analytical knowledges,",
    "ai-tha-ap476:5.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap476:5.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap476:6.1_0": "That is how these verses were recited by the senior venerable Susīma.",
    "ai-tha-ap476:7.1_0": "for the fourth Elder Sunetta"
}