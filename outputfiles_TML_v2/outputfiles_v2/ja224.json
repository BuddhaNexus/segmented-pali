{
    "ai-ja224:0.1_0": "Jātaka",
    "ai-ja224:0.2_0": "Dukanipāta",
    "ai-ja224:0.3_0": "Kāsāvavagga",
    "ai-ja224:0.4_0": "4. Kumbhilajātaka",
    "ai-ja224:1.1_0": "“In these four qualities",
    "ai-ja224:1.2_0": "like a flock of sandalwood,",
    "ai-ja224:1.3_0": "does not grieve after passing away:",
    "ai-ja224:1.4_0": "looks down on him,",
    "ai-ja224:2.1_0": "If there is no desire",
    "ai-ja224:2.2_0": "Endowed with the virtues of good qualities,",
    "ai-ja224:2.3_0": "does not grieve after passing away:",
    "ai-ja224:2.4_0": "does not transcend this.”",
    "ai-ja224:3.1_0": "and fourth in pot."
}