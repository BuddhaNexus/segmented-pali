{
    "ai-tha-ap527:0.1_0": "Therāpadāna",
    "ai-tha-ap527:0.2_0": "Tiṇadāyakavagga",
    "ai-tha-ap527:0.3_0": "5. Supaṭadāyakattheraapadāna",
    "ai-tha-ap527:1.1_0": "Leaving my day’s meditation",
    "ai-tha-ap527:1.2_0": "he sees the world’s end.",
    "ai-tha-ap527:1.3_0": "having given a good lady,",
    "ai-tha-ap527:1.4_0": "I’ll rejoice in heaven for an eon.’",
    "ai-tha-ap527:2.1_0": "A single eon irrevers:",
    "ai-tha-ap527:2.2_0": "I am fortunate,",
    "ai-tha-ap527:2.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap527:2.4_0": "this is the fruit for the dead.",
    "ai-tha-ap527:3.1_0": "corrupted by defilements,",
    "ai-tha-ap527:3.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap527:3.3_0": "and live without defilements.",
    "ai-tha-ap527:4.1_0": "It was so welcome for me",
    "ai-tha-ap527:4.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap527:4.3_0": "and fulfilled the Buddha’s instructions.",
    "ai-tha-ap527:5.1_0": "the four analytical knowledges,",
    "ai-tha-ap527:5.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap527:5.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap527:6.1_0": "That is how these verses were recited by the senior venerable Subhadda.",
    "ai-tha-ap527:7.1_0": "for the fifth time (whole) Subhadda donation was the fifth Elder Subhadda"
}