{
    "ai-tha-ap456:0.1_0": "Therāpadāna",
    "ai-tha-ap456:0.2_0": "Jagatidāyakavagga",
    "ai-tha-ap456:0.3_0": "4 Tiṇukkadhāriyattheraapadāna",
    "ai-tha-ap456:1.1_0": "“Padumuttara, the Buddha Padumuttara,",
    "ai-tha-ap456:1.2_0": "the Buddha-to-be,",
    "ai-tha-ap456:1.3_0": "confident in heart,",
    "ai-tha-ap456:1.4_0": "three was set up a forest grove.",
    "ai-tha-ap456:2.1_0": "it’s a hundred thousand eons",
    "ai-tha-ap456:2.2_0": "I dropped my stomach.",
    "ai-tha-ap456:2.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap456:2.4_0": "this is the fruit of giving.",
    "ai-tha-ap456:3.1_0": "corrupted by defilements,",
    "ai-tha-ap456:3.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap456:3.3_0": "and live without defilements.",
    "ai-tha-ap456:4.1_0": "It was so welcome for me",
    "ai-tha-ap456:4.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap456:4.3_0": "and fulfilled the Buddha’s instructions.",
    "ai-tha-ap456:5.1_0": "the four analytical knowledges,",
    "ai-tha-ap456:5.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap456:5.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap456:6.1_0": "That is how these verses were recited by the senior venerable Scriptures.",
    "ai-tha-ap456:7.1_0": "torch against the crossroads, to the fourth Elder Nun Sukkā"
}