{
    "ai-thig1.15:0.1_0": "Therīgāthā",
    "ai-thig1.15:0.2_0": "Ekakanipāta",
    "ai-thig1.15:0.3_0": "15. Uttarātherīgāthā",
    "ai-thig1.15:1.1_0": "I was restrained",
    "ai-thig1.15:1.2_0": "by body, speech, and mind—",
    "ai-thig1.15:1.3_0": "and plucked out craving, root and all,",
    "ai-thig1.15:1.4_0": "I have become cooled and quenched.",
    "ai-thig1.15:2.1_0": "Namely,"
}