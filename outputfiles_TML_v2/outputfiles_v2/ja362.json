{
    "ai-ja362:0.1_0": "Jātaka",
    "ai-ja362:0.2_0": "Pañcakanipāta",
    "ai-ja362:0.3_0": "Vaṇṇārohavagga",
    "ai-ja362:0.4_0": "2. Sīlavīmaṁsajātaka",
    "ai-ja362:1.1_0": "“Ethics is better for the best:",
    "ai-ja362:1.2_0": "Such was my close.",
    "ai-ja362:1.3_0": "Ethics are better for you,",
    "ai-ja362:1.4_0": "This is so.",
    "ai-ja362:2.1_0": "shone, beautiful,",
    "ai-ja362:2.2_0": "Ethics are the best for marriage.",
    "ai-ja362:2.3_0": "Established in ethics,",
    "ai-ja362:2.4_0": "For you’re not able to caught up in the charnel ground,",
    "ai-ja362:3.1_0": "An aristocrat, and unprincipled,",
    "ai-ja362:3.2_0": "and I stand in line with the teaching.",
    "ai-ja362:3.3_0": "After filling up the world’s streams,",
    "ai-ja362:3.4_0": "arise in the lower world,",
    "ai-ja362:4.1_0": "aristocrats, brahmins, merchants, and workers.",
    "ai-ja362:4.2_0": "workers, outcastes,",
    "ai-ja362:4.3_0": "Having practiced the teaching here,",
    "ai-ja362:4.4_0": "The Heaves are equal,",
    "ai-ja362:5.1_0": "hatred never ends in lives.",
    "ai-ja362:5.2_0": "nor does birth trapped in a net.",
    "ai-ja362:5.3_0": "their ethics are transmigration.",
    "ai-ja362:5.4_0": "and in the next.”",
    "ai-ja362:6.1_0": "(Through) virtue, the second (born) is the second (born)."
}