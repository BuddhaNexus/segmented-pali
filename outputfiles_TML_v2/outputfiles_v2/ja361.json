{
    "ai-ja361:0.1_0": "Jātaka",
    "ai-ja361:0.2_0": "Pañcakanipāta",
    "ai-ja361:0.3_0": "Vaṇṇārohavagga",
    "ai-ja361:0.4_0": "1. Vaṇṇārohajātaka",
    "ai-ja361:1.1_0": "“He’s afflicted with color,",
    "ai-ja361:1.2_0": "A person who works",
    "ai-ja361:1.3_0": "It’s not easy to see,",
    "ai-ja361:1.4_0": "you say so well-trained,”",
    "ai-ja361:2.1_0": "“He’s afflicted with color,",
    "ai-ja361:2.2_0": "A person who works",
    "ai-ja361:2.3_0": "I’ve heard:",
    "ai-ja361:2.4_0": "Well then, you say so well-spoken words:",
    "ai-ja361:3.1_0": "Wicked One, if you come near me",
    "ai-ja361:3.2_0": "You’re so hard to comprehend.",
    "ai-ja361:3.3_0": "But I don’t go with you,",
    "ai-ja361:3.4_0": "Practicing the Realized One,",
    "ai-ja361:4.1_0": "The one who understands the words of another,",
    "ai-ja361:4.2_0": "you should use them,",
    "ai-ja361:4.3_0": "quickly break your friends.",
    "ai-ja361:4.4_0": "trembled with enmity.",
    "ai-ja361:5.1_0": "He who is a friend should always be heedful,",
    "ai-ja361:5.2_0": "dwells contemplating the highway,",
    "ai-ja361:5.3_0": "Yasa, the son of Sīdhī,",
    "ai-ja361:5.4_0": "A friend should not be cut off from their friends.”",
    "ai-ja361:6.1_0": "For the first person embraced,"
}