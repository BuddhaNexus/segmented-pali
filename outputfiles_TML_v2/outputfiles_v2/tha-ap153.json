{
    "ai-tha-ap153:0.1_0": "Therāpadāna",
    "ai-tha-ap153:0.2_0": "Bandhujīvakavagga",
    "ai-tha-ap153:0.3_0": "1. Bandhujīvakattheraapadāna",
    "ai-tha-ap153:1.1_0": "“We who see the stainless, pure,",
    "ai-tha-ap153:1.2_0": "clear and undisturbed,",
    "ai-tha-ap153:1.3_0": "they’ve ended desire to be reborn:",
    "ai-tha-ap153:1.4_0": "he’s crossed over the world,",
    "ai-tha-ap153:2.1_0": "the people conquered.",
    "ai-tha-ap153:2.2_0": "the winspiringly.",
    "ai-tha-ap153:2.3_0": "I’m a sage in the forest,",
    "ai-tha-ap153:2.4_0": "meditate on the ugly aspects of the body:",
    "ai-tha-ap153:3.1_0": "Gravel and cast-oil.",
    "ai-tha-ap153:3.2_0": "A Perceiving",
    "ai-tha-ap153:3.3_0": "I enjoyed the Buddha.",
    "ai-tha-ap153:3.4_0": "Kinsman of the world,",
    "ai-tha-ap153:4.1_0": "It’s been thirty one eons",
    "ai-tha-ap153:4.2_0": "I wandered about the (good) actions.",
    "ai-tha-ap153:4.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap153:4.4_0": "this fruit is to know,",
    "ai-tha-ap153:5.1_0": "hairs beards the seven eons",
    "ai-tha-ap153:5.2_0": "a big crowd of people.",
    "ai-tha-ap153:5.3_0": "for the sake of all seers:",
    "ai-tha-ap153:5.4_0": "The Mahallika conversions",
    "ai-tha-ap153:6.1_0": "the four analytical knowledges,",
    "ai-tha-ap153:6.2_0": "having excellence and liberation,",
    "ai-tha-ap153:6.3_0": "I’ve realized the six kinds of direct knowledge,",
    "ai-tha-ap153:6.4_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap153:7.1_0": "That is how these verses were recited by the senior venerable Bandhumatī.",
    "ai-tha-ap153:8.1_0": "First the first Elder Nun Bandhumatī"
}