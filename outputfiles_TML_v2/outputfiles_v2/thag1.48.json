{
    "ai-thag1.48:0.1_0": "Theragāthā",
    "ai-thag1.48:0.2_0": "Ekakanipāta",
    "ai-thag1.48:0.3_0": "Pañcamavagga",
    "ai-thag1.48:0.4_0": "1.48. Sañjayattheragāthā",
    "ai-thag1.48:1.1_0": "Since I went forth",
    "ai-thag1.48:1.2_0": "from the lay life to homelessness.",
    "ai-thag1.48:1.3_0": "I’ve not been aware of any thought",
    "ai-thag1.48:1.4_0": "that is ignoble and hateful.",
    "ai-thag1.48:1.5_0": "Sañjaya,"
}