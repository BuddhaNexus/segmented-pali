{
    "ai-thag2.28:0.1_0": "Theragāthā",
    "ai-thag2.28:0.2_0": "Dukanipāta",
    "ai-thag2.28:0.3_0": "Tatiyavagga",
    "ai-thag2.28:0.4_0": "2.28. Bharatattheragāthā",
    "ai-thag2.28:1.1_0": "Come, Nandaka, let’s go",
    "ai-thag2.28:1.2_0": "to visit our preceptor.",
    "ai-thag2.28:1.3_0": "We’ll roar our lion’s roar",
    "ai-thag2.28:1.4_0": "before the best of Buddhas.",
    "ai-thag2.28:2.1_0": "Out of compassion for us,",
    "ai-thag2.28:2.2_0": "out of compassion, so we could realize",
    "ai-thag2.28:2.3_0": "the ending of all fetters—",
    "ai-thag2.28:2.4_0": "the ending of all fetters.",
    "ai-thag2.28:2.5_0": "... a senior monk ..."
}