{
    "ai-ja322:0.1_0": "Jātaka",
    "ai-ja322:0.2_0": "Catukkanipāta",
    "ai-ja322:0.3_0": "Kuṭidūsakavagga",
    "ai-ja322:0.4_0": "2. Duddubhajātaka",
    "ai-ja322:1.1_0": "“Venerable sir, very hard to see you.",
    "ai-ja322:1.2_0": "In that case I’m teaching,",
    "ai-ja322:1.3_0": "I don’t know that.",
    "ai-ja322:1.4_0": "what’s the hard to see?”",
    "ai-ja322:2.1_0": "“Having heard a woodland grove",
    "ai-ja322:2.2_0": "smooth in glist:",
    "ai-ja322:2.3_0": "When they hear their words,",
    "ai-ja322:2.4_0": "caught up in the trapper to lament.",
    "ai-ja322:3.1_0": "Without having cognised the truth,",
    "ai-ja322:3.2_0": "attacking the followers of others;",
    "ai-ja322:3.3_0": "Those fools (surely) fools,",
    "ai-ja322:3.4_0": "so they are skilful in another’s duties.",
    "ai-ja322:4.1_0": "Those who are accomplished in ethics,",
    "ai-ja322:4.2_0": "delight in peace.",
    "ai-ja322:4.3_0": "The wise and learned,",
    "ai-ja322:4.4_0": "it doesn’t grieve for others.”",
    "ai-ja322:5.1_0": "The second bubble was born."
}