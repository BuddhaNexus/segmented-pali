{
    "ai-cp28:0.1_0": "Cariyāpiṭaka",
    "ai-cp28:0.2_0": "Yudhañjayavagga",
    "ai-cp28:0.3_0": "Saccapāramī 2",
    "ai-cp28:0.4_0": "Saccatāpasacariya",
    "ai-cp28:1.1_0": "“I’m not ever to ask for a question,",
    "ai-cp28:1.2_0": "He has realized the Truth.",
    "ai-cp28:1.3_0": "truths the world,",
    "ai-cp28:1.4_0": "a harmonious person.”",
    "ai-cp28:2.1_0": "she lived on the eighth day."
