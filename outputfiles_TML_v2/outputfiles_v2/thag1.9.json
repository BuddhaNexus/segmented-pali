{
    "ai-thag1.9:0.1_0": "Theragāthā",
    "ai-thag1.9:0.2_0": "Ekakanipāta",
    "ai-thag1.9:0.3_0": "Paṭhamavagga",
    "ai-thag1.9:0.4_0": "1.9 Pilindavacchattheragāthā",
    "ai-thag1.9:1.1_0": "It was welcome, not unwelcome,",
    "ai-thag1.9:1.2_0": "the advice I got was good.",
    "ai-thag1.9:1.3_0": "Of things which are shared,",
    "ai-thag1.9:1.4_0": "I encountered the best.",
    "ai-thag1.9:1.5_0": "That is how this verse was recited by the senior venerable Pilindavaccha."
}