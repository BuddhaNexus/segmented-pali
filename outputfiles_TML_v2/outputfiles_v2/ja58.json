{
    "ai-ja58:0.1_0": "Jātaka",
    "ai-ja58:0.2_0": "Ekakanipāta",
    "ai-ja58:0.3_0": "Āsīsavagga",
    "ai-ja58:0.4_0": "8. Tayodhammajātaka",
    "ai-ja58:1.1_0": "“These three things",
    "ai-ja58:1.2_0": "like a flock of sandalwood,",
    "ai-ja58:1.3_0": "Knowing the sun,",
    "ai-ja58:1.4_0": "they’ve seen this.”",
    "ai-ja58:2.1_0": "was the eighth Dhamma."
}