{
    "ai-thag1.30:0.1_0": "Theragāthā",
    "ai-thag1.30:0.2_0": "Ekakanipāta",
    "ai-thag1.30:0.3_0": "Tatiyavagga",
    "ai-thag1.30:0.4_0": "1.30. Uttiyattheragāthā",
    "ai-thag1.30:1.1_0": "When I was ill in the past,",
    "ai-thag1.30:1.2_0": "mindfulness arose in me.",
    "ai-thag1.30:1.3_0": "Now I am ill once more—",
    "ai-thag1.30:1.4_0": "there is no time for me to be careless.”",
    "ai-thag1.30:1.5_0": "…",
    "ai-thag1.30:1.6_0": "senior monks,",
    "ai-thag1.30:1.7_0": "The third sub-chapter on lying down is finished.",
    "ai-thag1.30:2.0_0": "“This is the summary:",
    "ai-thag1.30:2.1_0": "the senior monk who is immoral,",
    "ai-thag1.30:2.2_0": "The Elder Godhā",
    "ai-thag1.30:2.3_0": "The Elder Nandiya, having fear,",
    "ai-thag1.30:2.4_0": "The elder’s body is renowned,",
    "ai-thag1.30:2.5_0": "Jambukhādaka,",
    "ai-thag1.30:2.6_0": "Archery was called Isis."
}