{
    "ai-tha-ap76:0.1_0": "Therāpadāna",
    "ai-tha-ap76:0.2_0": "Nāgasamālavagga",
    "ai-tha-ap76:0.3_0": "My Elder Māluṅkyaputta",
    "ai-tha-ap76:1.1_0": "“You’re running the forest,",
    "ai-tha-ap76:1.2_0": "I’ll stay in the same room.",
    "ai-tha-ap76:1.3_0": "I saw the Buddha,",
    "ai-tha-ap76:1.4_0": "receiver of sacrifices,",
    "ai-tha-ap76:2.1_0": "I took a mattress,",
    "ai-tha-ap76:2.2_0": "washed the water;",
    "ai-tha-ap76:2.3_0": "I paid homage at my feet,",
    "ai-tha-ap76:2.4_0": "he went away.",
    "ai-tha-ap76:3.1_0": "A single eon irrevers:",
    "ai-tha-ap76:3.2_0": "like a broken-down jungle,",
    "ai-tha-ap76:3.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap76:3.4_0": "the fruit of good deeds.",
    "ai-tha-ap76:4.1_0": "and on the third day they were told,",
    "ai-tha-ap76:4.2_0": "Feat, spotless,",
    "ai-tha-ap76:4.3_0": "endowed with the seven treasures,",
    "ai-tha-ap76:4.4_0": "The Mahallika conversions",
    "ai-tha-ap76:5.1_0": "the four analytical knowledges,",
    "ai-tha-ap76:5.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap76:5.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap76:6.1_0": "That is how these verses were recited by the senior venerable Bāluṅkyaputta.",
    "ai-tha-ap76:7.1_0": "Having broken terror",
    "ai-tha-ap76:7.2_0": "The sixth section for recitation is finished."
}