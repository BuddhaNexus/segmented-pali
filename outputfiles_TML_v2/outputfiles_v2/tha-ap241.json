{
    "ai-tha-ap241:0.1_0": "Therāpadāna",
    "ai-tha-ap241:0.2_0": "Udakāsanavagga",
    "ai-tha-ap241:0.3_0": "9. Jagatikārakattheraapadāna",
    "ai-tha-ap241:1.1_0": "“I’m extinguished in the world,",
    "ai-tha-ap241:1.2_0": "you’re not able to see the goal.",
    "ai-tha-ap241:1.3_0": "the elder’s father was guarded by me.",
    "ai-tha-ap241:1.4_0": "the supreme Buddha.",
    "ai-tha-ap241:2.1_0": "There are eighteen eons",
    "ai-tha-ap241:2.2_0": "I wandered about the (good) actions.",
    "ai-tha-ap241:2.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap241:2.4_0": "this fruit is the fruit.",
    "ai-tha-ap241:3.1_0": "the four analytical knowledges,",
    "ai-tha-ap241:3.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap241:3.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap241:4.1_0": "That is how these verses were recited by the senior venerable Jotipāla.",
    "ai-tha-ap241:5.1_0": "Jotipāla’s senior"
}