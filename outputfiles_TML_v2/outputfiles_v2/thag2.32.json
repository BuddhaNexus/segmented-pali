{
    "ai-thag2.32:0.1_0": "Theragāthā",
    "ai-thag2.32:0.2_0": "Dukanipāta",
    "ai-thag2.32:0.3_0": "Catutthavagga",
    "ai-thag2.32:0.4_0": "2.32 Sivakattheragāthā",
    "ai-thag2.32:1.1_0": "Houses are impermanent—",
    "ai-thag2.32:1.2_0": "on and on, life after life.",
    "ai-thag2.32:1.3_0": "searching for the house-builder;",
    "ai-thag2.32:1.4_0": "Painful is birth again and again.",
    "ai-thag2.32:2.1_0": "where you’ve been seen someone,",
    "ai-thag2.32:2.2_0": "You won’t build a house again.",
    "ai-thag2.32:2.3_0": "Your rafters are all broken,",
    "ai-thag2.32:2.4_0": "your ridgepole is shattered.",
    "ai-thag2.32:2.5_0": "my mind is clear, made up,",
    "ai-thag2.32:2.6_0": "will disrobe right here.”",
    "ai-thag2.32:2.7_0": "Sivaka,"
}