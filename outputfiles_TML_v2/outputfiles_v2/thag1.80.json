{
    "ai-thag1.80:0.1_0": "Theragāthā",
    "ai-thag1.80:0.2_0": "Ekakanipāta",
    "ai-thag1.80:0.3_0": "Aṭṭhamavagga",
    "ai-thag1.80:0.4_0": "1.80. Uggattheragāthā",
    "ai-thag1.80:1.1_0": "Whatever actions I have performed,",
    "ai-thag1.80:1.2_0": "whether trivial or important,",
    "ai-thag1.80:1.3_0": "are all completely exhausted:",
    "ai-thag1.80:1.4_0": "now there’ll be no more future lives.”",
    "ai-thag1.80:1.5_0": "…",
    "ai-thag1.80:1.6_0": "Ugga ofabha ...",
    "ai-thag1.80:1.7_0": "The eighth sub-chapter on lying down is finished.",
    "ai-thag1.80:2.0_0": "“This is the summary:",
    "ai-thag1.80:2.1_0": "the senior monk Vaccha,",
    "ai-thag1.80:2.2_0": "the student Isisiṅga’s.",
    "ai-thag1.80:2.3_0": "Sumana, Subhadda,",
    "ai-thag1.80:2.4_0": "The Elder, (god) present, he who suffered them,",
    "ai-thag1.80:2.5_0": "You’re a son of Meṇḍaka,",
    "ai-thag1.80:2.6_0": "guarded by protection."
}