{
    "ai-ja172:0.1_0": "Jātaka",
    "ai-ja172:0.2_0": "Dukanipāta",
    "ai-ja172:0.3_0": "Kalyāṇavagga",
    "ai-ja172:0.4_0": "2. Daddarajātaka",
    "ai-ja172:1.1_0": "“Now who, great noise,",
    "ai-ja172:1.2_0": "gives to a servant,",
    "ai-ja172:1.3_0": "They don’t practice the way",
    "ai-ja172:1.4_0": "Who is the best of Buddhas?”",
    "ai-ja172:2.1_0": "“The trapper of deer is ruined,",
    "ai-ja172:2.2_0": "Piṅgiyānī Rains",
    "ai-ja172:2.3_0": "of birth and death,",
    "ai-ja172:2.4_0": "remains silent.”",
    "ai-ja172:3.1_0": "with the second border of curd,"
}