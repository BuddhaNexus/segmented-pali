{
    "ai-ja168:0.1_0": "Jātaka",
    "ai-ja168:0.2_0": "Dukanipāta",
    "ai-ja168:0.3_0": "Santhavavagga",
    "ai-ja168:0.4_0": "8. Sakuṇagghijātaka",
    "ai-ja168:1.1_0": "“A hawk diving the army,",
    "ai-ja168:1.2_0": "he would wander about evening,",
    "ai-ja168:1.3_0": "with violence.",
    "ai-ja168:1.4_0": "they’ll go to death.",
    "ai-ja168:2.1_0": "I’m not accomplished in knowledge,",
    "ai-ja168:2.2_0": "roaming to the ghost realm,",
    "ai-ja168:2.3_0": "May I rejoice!",
    "ai-ja168:2.4_0": "out of regard for my own welfare.”",
    "ai-ja168:3.1_0": "the eighth bird was wings away in the eighth day."
}