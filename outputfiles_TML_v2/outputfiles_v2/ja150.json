{
    "ai-ja150:0.1_0": "Jātaka",
    "ai-ja150:0.2_0": "Ekakanipāta",
    "ai-ja150:0.3_0": "Kakaṇṭakavagga",
    "ai-ja150:0.4_0": "10. Sañjīvajātaka",
    "ai-ja150:1.1_0": "“The one who attains supreme,",
    "ai-ja150:1.2_0": "it associates with the bad,",
    "ai-ja150:1.3_0": "which brought them up.",
    "ai-ja150:1.4_0": "like a shaven head.”",
    "ai-ja150:2.1_0": "The tenth and Queen Sañjīva",
    "ai-ja150:2.2_0": "Kaṇṭaka, fifteenth and eighth day,",
    "ai-ja150:3.1_0": "This is the summary:",
    "ai-ja150:4.1_0": "prospers in happiness and broth.",
    "ai-ja150:4.2_0": "like the wind from the heap of Tree",
    "ai-ja150:4.3_0": "Having made an even territory again, and again rejoicing",
    "ai-ja150:4.4_0": "if he obtains a maximum of four divisions (four);",
    "ai-ja150:5.1_0": "above, heels,",
    "ai-ja150:6.1_0": "Then the Vaggumudā",
    "ai-ja150:7.1_0": "Well-Puṇṇaka, Lady,",
    "ai-ja150:7.2_0": "The fifth benefit.",
    "ai-ja150:7.3_0": "The woman’s manlikes",
    "ai-ja150:7.4_0": "Ten on the tenth day.",
    "ai-ja150:7.5_0": "Calm in others, made laziness.",
    "ai-ja150:7.6_0": "Fire is the best of thorns,",
    "ai-ja150:8.1_0": "and made a canopy.",
    "ai-ja150:9.1_0": "The Book of the Ones is finished."
}