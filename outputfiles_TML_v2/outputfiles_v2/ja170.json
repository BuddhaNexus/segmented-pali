{
    "ai-ja170:0.1_0": "Jātaka",
    "ai-ja170:0.2_0": "Dukanipāta",
    "ai-ja170:0.3_0": "Santhavavagga",
    "ai-ja170:0.4_0": "10. Kakaṇṭakajātaka",
    "ai-ja170:1.1_0": "“In the past I wasn’t risen,",
    "ai-ja170:1.2_0": "Toraṇavatthu, with the householder,",
    "ai-ja170:1.3_0": "illustrious and loose,",
    "ai-ja170:1.4_0": "In what way is a thorny drum?”",
    "ai-ja170:2.1_0": "“Having received before,",
    "ai-ja170:2.2_0": "a fortnight.",
    "ai-ja170:2.3_0": "thought, “This is the sign of the King.”",
    "ai-ja170:2.4_0": "hanging my head.”",
    "ai-ja170:3.1_0": "The tenth thorns of Kaṇṭaka.",
    "ai-ja170:3.2_0": "The second sub-chapter on blankets is finished.",
    "ai-ja170:4.1_0": "This is the summary:",
    "ai-ja170:5.1_0": "Then in the Indadāla cave,",
    "ai-ja170:5.2_0": "Sumana, Timbaru, Victor,",
    "ai-ja170:5.3_0": "The monks Upavāḷa Sara",
    "ai-ja170:5.4_0": "Then with loving-kindness"
}