{
    "ai-tha-ap224:0.1_0": "Therāpadāna",
    "ai-tha-ap224:0.2_0": "Ālambaṇadāyakavagga",
    "ai-tha-ap224:0.3_0": "2. Ajinadāyakattheraapadāna",
    "ai-tha-ap224:1.1_0": "“King, tormenting;",
    "ai-tha-ap224:1.2_0": "I’m the one who takes the goal.",
    "ai-tha-ap224:1.3_0": "I saw the stainless Buddha",
    "ai-tha-ap224:1.4_0": "receiver of sacrifices,",
    "ai-tha-ap224:2.1_0": "I’ve given a string.",
    "ai-tha-ap224:2.2_0": "Kinsman of the world,",
    "ai-tha-ap224:2.3_0": "Through that deed and its result,",
    "ai-tha-ap224:2.4_0": "nor in the world’s end.",
    "ai-tha-ap224:3.1_0": "enjoying the attainment of existence,",
    "ai-tha-ap224:3.2_0": "I’m the practicing absorption,",
    "ai-tha-ap224:3.3_0": "I bear their final body,",
    "ai-tha-ap224:3.4_0": "in the teaching of the Buddha,",
    "ai-tha-ap224:4.1_0": "It’s been thirty one eons",
    "ai-tha-ap224:4.2_0": "with the victor’s instructions.",
    "ai-tha-ap224:4.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap224:4.4_0": "this is the fruit of Ajita.",
    "ai-tha-ap224:5.1_0": "hair five eons",
    "ai-tha-ap224:5.2_0": "I was a king of Sudassana,",
    "ai-tha-ap224:5.3_0": "endowed with the seven treasures,",
    "ai-tha-ap224:5.4_0": "The Mahallika conversions",
    "ai-tha-ap224:6.1_0": "the four analytical knowledges,",
    "ai-tha-ap224:6.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap224:6.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap224:7.1_0": "That is how these verses were recited by the senior venerable Ajita.",
    "ai-tha-ap224:8.1_0": "The second universal Elder Ajita"
}