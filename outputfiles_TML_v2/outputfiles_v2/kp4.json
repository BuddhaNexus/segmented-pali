{
    "ai-kp4:0.1_0": "Khuddakapāṭha",
    "ai-kp4:0.2_0": "4. Kumārapañhā",
    "ai-kp4:1.1_0": "What is the one?",
    "ai-kp4:1.2_0": "All sentient beings are sustained by food.",
    "ai-kp4:2.1_0": "What is the two?",
    "ai-kp4:2.2_0": "Mind and body.",
    "ai-kp4:3.1_0": "What is the three?",
    "ai-kp4:3.2_0": "Three feelings.",
    "ai-kp4:4.1_0": "What is the four?",
    "ai-kp4:4.2_0": "the four noble truths,",
    "ai-kp4:5.1_0": "What is the five?",
    "ai-kp4:5.2_0": "Five grasping aggregates.",
    "ai-kp4:6.1_0": "What is the six?",
    "ai-kp4:6.2_0": "Six interior sense fields.",
    "ai-kp4:7.1_0": "What is the seven?",
    "ai-kp4:7.2_0": "The seven awakening factors.",
    "ai-kp4:8.1_0": "What is the eight?",
    "ai-kp4:8.2_0": "it is the noble eightfold path—",
    "ai-kp4:9.1_0": "What is the nine?",
    "ai-kp4:9.2_0": "Nine abodes of sentient beings.",
    "ai-kp4:10.1_0": "What is the ten?",
    "ai-kp4:10.2_0": "One endowed with ten factors is called “perfected”.",
    "ai-kp4:11.1_0": "The Boy’s Questions"
