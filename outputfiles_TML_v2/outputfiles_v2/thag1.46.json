{
    "ai-thag1.46:0.1_0": "Theragāthā",
    "ai-thag1.46:0.2_0": "Ekakanipāta",
    "ai-thag1.46:0.3_0": "Pañcamavagga",
    "ai-thag1.46:0.4_0": "1.46. Samiddhittheragāthā",
    "ai-thag1.46:1.1_0": "I went forth out of faith",
    "ai-thag1.46:1.2_0": "from the lay life to homelessness.",
    "ai-thag1.46:1.3_0": "My mindfulness and wisdom have grown,",
    "ai-thag1.46:1.4_0": "A mind that’s serene,",
    "ai-thag1.46:1.5_0": "Make whatever illusions you want,",
    "ai-thag1.46:1.6_0": "it won’t bother me.”",
    "ai-thag1.46:1.7_0": "... the Elder Samiddhi ..."
}