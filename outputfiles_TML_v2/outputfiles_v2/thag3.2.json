{
    "ai-thag3.2:0.1_0": "Theragāthā",
    "ai-thag3.2:0.2_0": "Tikanipāta",
    "ai-thag3.2:0.3_0": "Paṭhamavagga",
    "ai-thag3.2:0.4_0": "3.2. Paccayattheragāthā",
    "ai-thag3.2:1.1_0": "I went forth five days ago,",
    "ai-thag3.2:1.2_0": "my heart’s desire is still unfulfilled.",
    "ai-thag3.2:1.3_0": "I entered my dwelling",
    "ai-thag3.2:1.4_0": "was like the goal.",
    "ai-thag3.2:2.1_0": "I won’t eat, I won’t drink,",
    "ai-thag3.2:2.2_0": "I won’t leave my dwelling;",
    "ai-thag3.2:2.3_0": "nor will I lie down on my side—",
    "ai-thag3.2:2.4_0": "not until the dart of craving is drawn out.",
    "ai-thag3.2:3.1_0": "See my energy and vigor",
    "ai-thag3.2:3.2_0": "see my energy and vigor!",
    "ai-thag3.2:3.3_0": "I’ve attained the three knowledges;",
    "ai-thag3.2:3.4_0": "and fulfilled the Buddha’s instructions.",
    "ai-thag3.2:3.5_0": "... the senior monks ..."
}