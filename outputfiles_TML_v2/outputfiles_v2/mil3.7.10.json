{
    "ai-mil3.7.10:0.1_0": "Milindapañha",
    "ai-mil3.7.10:0.2_0": "Arūpadhammavavatthānavagga",
    "ai-mil3.7.10:0.3_0": "10. Dīghaṭṭhipañha",
    "ai-mil3.7.10:1.1_0": "The King said,",
    "ai-mil3.7.10:1.2_0": "“Venerable Sir, you must say this:",
    "ai-mil3.7.10:1.3_0": "how much more than one hundred leagues.",
    "ai-mil3.7.10:2.1_0": "“What do you think, great king?”",
    "ai-mil3.7.10:2.2_0": "“Yes, Sir.”",
    "ai-mil3.7.10:2.3_0": "“Great king, don’t this long 40 kilometers will last for one hundred leagues?”",
    "ai-mil3.7.10:3.1_0": "“Sir, has an army?”",
    "ai-mil3.7.10:4.1_0": "The tenth encircling is finished."
}