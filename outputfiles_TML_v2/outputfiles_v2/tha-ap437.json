{
    "ai-tha-ap437:0.1_0": "Therāpadāna",
    "ai-tha-ap437:0.2_0": "Ekavihārivagga",
    "ai-tha-ap437:0.3_0": "5. Ucchukhaṇḍikattheraapadāna",
    "ai-tha-ap437:1.1_0": "“You’re not bound with a harem,",
    "ai-tha-ap437:1.2_0": "the doors to the Deathless,",
    "ai-tha-ap437:1.3_0": "I saw the stainless Buddha",
    "ai-tha-ap437:1.4_0": "gone beyond all things.",
    "ai-tha-ap437:2.1_0": "including the “finish”",
    "ai-tha-ap437:2.2_0": "before the best of Buddhas.",
    "ai-tha-ap437:2.3_0": "confident in heart,",
    "ai-tha-ap437:2.4_0": "such a one, the great hermit,",
    "ai-tha-ap437:3.1_0": "A single eon irrevers:",
    "ai-tha-ap437:3.2_0": "When I ask you the net of giving birth,",
    "ai-tha-ap437:3.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap437:3.4_0": "this barren field is fruit.",
    "ai-tha-ap437:4.1_0": "corrupted by defilements,",
    "ai-tha-ap437:4.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap437:4.3_0": "and live without defilements.",
    "ai-tha-ap437:5.1_0": "It was so welcome for me",
    "ai-tha-ap437:5.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap437:5.3_0": "and fulfilled the Buddha’s instructions.",
    "ai-tha-ap437:6.1_0": "the four analytical knowledges,",
    "ai-tha-ap437:6.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap437:6.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap437:7.1_0": "That is how these verses were recited by the senior venerable Poison.",
    "ai-tha-ap437:8.1_0": "The fifth Elder"
}