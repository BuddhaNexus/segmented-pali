{
    "ai-thag3.11:0.1_0": "Theragāthā",
    "ai-thag3.11:0.2_0": "Tikanipāta",
    "ai-thag3.11:0.3_0": "Paṭhamavagga",
    "ai-thag3.11:0.4_0": "3.11. Upālittheragāthā",
    "ai-thag3.11:1.1_0": "One newly gone forth,",
    "ai-thag3.11:1.2_0": "Nine newly gone forth,",
    "ai-thag3.11:1.3_0": "should mix with spiritual friends,",
    "ai-thag3.11:1.4_0": "who are pure and livelihood,",
    "ai-thag3.11:2.1_0": "One newly gone forth,",
    "ai-thag3.11:2.2_0": "Nine newly gone forth,",
    "ai-thag3.11:2.3_0": "a mendicant staying in the Saṅgha,",
    "ai-thag3.11:2.4_0": "being wise, would train in monastic discipline.",
    "ai-thag3.11:3.1_0": "One newly gone forth,",
    "ai-thag3.11:3.2_0": "Nine newly gone forth,",
    "ai-thag3.11:3.3_0": "skillful intentions have been guarded by the eons.",
    "ai-thag3.11:3.4_0": "would wander undistracted.",
    "ai-thag3.11:3.5_0": "..."
}