{
    "ai-tha-ap233:0.1_0": "Therāpadāna",
    "ai-tha-ap233:0.2_0": "Udakāsanavagga",
    "ai-tha-ap233:0.3_0": "The Elder Udaya",
    "ai-tha-ap233:1.1_0": "“Having gone forth in the monastery, they have no sense of urgency,",
    "ai-tha-ap233:1.2_0": "I strew a floor.",
    "ai-tha-ap233:1.3_0": "I prepare the water;",
    "ai-tha-ap233:1.4_0": "as proper attention.",
    "ai-tha-ap233:2.1_0": "It’s been thirty one eons",
    "ai-tha-ap233:2.2_0": "I wandered about the (good) actions.",
    "ai-tha-ap233:2.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap233:2.4_0": "on a seat spread out.",
    "ai-tha-ap233:3.1_0": "in the fifteenth day sabbath,",
    "ai-tha-ap233:3.2_0": "recormmended by Path.",
    "ai-tha-ap233:3.3_0": "endowed with the seven treasures,",
    "ai-tha-ap233:3.4_0": "The Mahallika conversions",
    "ai-tha-ap233:4.1_0": "the four analytical knowledges,",
    "ai-tha-ap233:4.2_0": "having excellence and liberation,",
    "ai-tha-ap233:4.3_0": "I’ve realized the six kinds of direct knowledge,",
    "ai-tha-ap233:4.4_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap233:5.1_0": "That is how these verses were recited by the senior venerable Udaya.",
    "ai-tha-ap233:6.1_0": "first Elder Udaya’s Udaya"
}