{
    "ai-tha-ap310:0.1_0": "Therāpadāna",
    "ai-tha-ap310:0.2_0": "Padumakesaravagga",
    "ai-tha-ap310:0.3_0": "8. Anulepadāyakattheraapadāna",
    "ai-tha-ap310:1.1_0": "“I’ll realize the goal",
    "ai-tha-ap310:1.2_0": "I saw a disciple,",
    "ai-tha-ap310:1.3_0": "and after doing building work,",
    "ai-tha-ap310:1.4_0": "I’ll quickly get close to him.",
    "ai-tha-ap310:2.1_0": "and nine sections are finished,",
    "ai-tha-ap310:2.2_0": "Let us make an effort",
    "ai-tha-ap310:2.3_0": "confident in heart,",
    "ai-tha-ap310:2.4_0": "the supreme field of merit—",
    "ai-tha-ap310:3.1_0": "There are eighteen eons",
    "ai-tha-ap310:3.2_0": "I wandered about the (good) actions.",
    "ai-tha-ap310:3.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap310:3.4_0": "looking forward to the fruit,",
    "ai-tha-ap310:4.1_0": "the four analytical knowledges,",
    "ai-tha-ap310:4.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap310:4.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap310:5.1_0": "That is how these verses were recited by the senior venerable Protector.",
    "ai-tha-ap310:6.1_0": "for the eighth year (where the Elder Nuns)"
}