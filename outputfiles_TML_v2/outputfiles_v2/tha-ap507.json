{
    "ai-tha-ap507:0.1_0": "Therāpadāna",
    "ai-tha-ap507:0.2_0": "Kaṇikāravagga",
    "ai-tha-ap507:0.3_0": "5 Pādaphaliyattheraapadāna",
    "ai-tha-ap507:1.1_0": "“A Buddha praised the appearance of a golden palanquin,",
    "ai-tha-ap507:1.2_0": "receiver of sacrifices,",
    "ai-tha-ap507:1.3_0": "keep it safe.",
    "ai-tha-ap507:1.4_0": "Footprints on the foot,",
    "ai-tha-ap507:2.1_0": "A single eon irreversible eons",
    "ai-tha-ap507:2.2_0": "the fruitless (of rebirth).",
    "ai-tha-ap507:2.3_0": "I’m not aware of any living being",
    "ai-tha-ap507:2.4_0": "as fruit is the fruit.",
    "ai-tha-ap507:3.1_0": "corrupted by defilements,",
    "ai-tha-ap507:3.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap507:3.3_0": "and live without defilements.",
    "ai-tha-ap507:4.1_0": "It was so welcome for me",
    "ai-tha-ap507:4.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap507:4.3_0": "and fulfilled the Buddha’s instructions.",
    "ai-tha-ap507:5.1_0": "the four analytical knowledges,",
    "ai-tha-ap507:5.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap507:5.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap507:6.1_0": "That is how these verses were recited by the senior venerable Pañcasikha.",
    "ai-tha-ap507:7.1_0": "The fifth Elder Nun Mettiya"
}