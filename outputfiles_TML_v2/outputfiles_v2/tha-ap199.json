{
    "ai-tha-ap199:0.1_0": "Therāpadāna",
    "ai-tha-ap199:0.2_0": "Tamālapupphiyavagga",
    "ai-tha-ap199:0.3_0": "7. Tindukadāyakattheraapadāna",
    "ai-tha-ap199:1.1_0": "“I went to a hillside,",
    "ai-tha-ap199:1.2_0": "heeld the monkey, who is great and small,",
    "ai-tha-ap199:1.3_0": "Seeing the fruit of the very (true) fruit.",
    "ai-tha-ap199:1.4_0": "recollecting the Buddha,",
    "ai-tha-ap199:2.1_0": "After leaving the place where,",
    "ai-tha-ap199:2.2_0": "for the sake of the world,",
    "ai-tha-ap199:2.3_0": "confident in heart,",
    "ai-tha-ap199:2.4_0": "the current:",
    "ai-tha-ap199:3.1_0": "Knowing my thoughts,",
    "ai-tha-ap199:3.2_0": "the supreme Teacher in the world",
    "ai-tha-ap199:3.3_0": "The thousand pollutants",
    "ai-tha-ap199:3.4_0": "I’ve come into my presence,",
    "ai-tha-ap199:4.1_0": "I’ve joyed the people,",
    "ai-tha-ap199:4.2_0": "I’ve come to fruit.",
    "ai-tha-ap199:4.3_0": "The Buddha received it.",
    "ai-tha-ap199:4.4_0": "all-knowing, speaking.",
    "ai-tha-ap199:5.1_0": "if he makes his four eons;",
    "ai-tha-ap199:5.2_0": "the fruitless (of rebirth).",
    "ai-tha-ap199:5.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap199:5.4_0": "as fruit is the fruit.",
    "ai-tha-ap199:6.1_0": "I’m the one who understands",
    "ai-tha-ap199:6.2_0": "It is a response,",
    "ai-tha-ap199:6.3_0": "endowed with the seven treasures,",
    "ai-tha-ap199:6.4_0": "The Mahallika conversions",
    "ai-tha-ap199:7.1_0": "the four analytical knowledges,",
    "ai-tha-ap199:7.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap199:7.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap199:8.1_0": "That is how these verses were recited by the senior venerable Nandaka.",
    "ai-tha-ap199:9.1_0": "the seventh Elder Tissaka"
}