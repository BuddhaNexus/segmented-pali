{
    "ai-tha-ap232:0.1_0": "Therāpadāna",
    "ai-tha-ap232:0.2_0": "Ālambaṇadāyakavagga",
    "ai-tha-ap232:0.3_0": "10 Ekavandaniyattheraapadāna",
    "ai-tha-ap232:1.1_0": "A bully creates a human broad.",
    "ai-tha-ap232:1.2_0": "in the victor’s teaching.",
    "ai-tha-ap232:1.3_0": "confident in heart,",
    "ai-tha-ap232:1.4_0": "the Buddha, the best of men,",
    "ai-tha-ap232:2.1_0": "It’s been thirty one eons",
    "ai-tha-ap232:2.2_0": "I wandered about the (good) actions.",
    "ai-tha-ap232:2.3_0": "I’m not aware of any living creatures.",
    "ai-tha-ap232:2.4_0": "this is the fruit,",
    "ai-tha-ap232:3.1_0": "In twenty-four it",
    "ai-tha-ap232:3.2_0": "and rotten.",
    "ai-tha-ap232:3.3_0": "endowed with the seven treasures,",
    "ai-tha-ap232:3.4_0": "The Mahallika conversions",
    "ai-tha-ap232:4.1_0": "the four analytical knowledges,",
    "ai-tha-ap232:4.2_0": "... “I’ll sleep ...”",
    "ai-tha-ap232:4.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap232:5.1_0": "That is how these verses were recited by the senior venerable Piṅgiya.",
    "ai-tha-ap232:6.1_0": "This one Elder Tissa",
    "ai-tha-ap232:7.1_0": "Jambukhādaka was twenty-three.",
    "ai-tha-ap232:8.1_0": "This is the summary:",
    "ai-tha-ap232:9.1_0": "Ajita, and Ajita,",
    "ai-tha-ap232:9.2_0": "he gave meat,",
    "ai-tha-ap232:9.3_0": "Endowed with good will,",
    "ai-tha-ap232:9.4_0": "The Simile of the Migāḷava,",
    "ai-tha-ap232:9.5_0": "The five verses spoken of singingly:",
    "ai-tha-ap232:9.6_0": "you see the meaning."
}