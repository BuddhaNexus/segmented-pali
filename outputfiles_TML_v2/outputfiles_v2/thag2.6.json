{
    "ai-thag2.6:0.1_0": "Theragāthā",
    "ai-thag2.6:0.2_0": "Dukanipāta",
    "ai-thag2.6:0.3_0": "Paṭhamavagga",
    "ai-thag2.6:0.4_0": "2.6 Meḷajinattheragāthā",
    "ai-thag2.6:1.1_0": "When I heard the Teacher",
    "ai-thag2.6:1.2_0": "speaking Dhamma,",
    "ai-thag2.6:1.3_0": "I wasn’t aware of any doubt",
    "ai-thag2.6:1.4_0": "in the all-knowing, unconquered one,",
    "ai-thag2.6:2.1_0": "the great hero, crooks,",
    "ai-thag2.6:2.2_0": "the most excellent of charioteers.",
    "ai-thag2.6:2.3_0": "I have no doubt",
    "ai-thag2.6:2.4_0": "in the path or practice.",
    "ai-thag2.6:2.5_0": "Meḷavī ..."
}