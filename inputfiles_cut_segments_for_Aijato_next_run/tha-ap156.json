{
  "tha-ap156:0.1_0": "Therāpadāna",
  "tha-ap156:0.2_0": "Bandhujīvakavagga",
  "tha-ap156:0.3_0": "4 Kakkārupupphapūjakattheraapadāna",
  "tha-ap156:1.1_0": "“Devaputto ahaṁ santo,",
  "tha-ap156:1.2_0": "pūjayiṁ sikhināyakaṁ;",
  "tha-ap156:1.3_0": "Kakkārupupphaṁ paggayha,",
  "tha-ap156:1.4_0": "buddhassa abhiropayiṁ.",
  "tha-ap156:2.1_0": "Ekattiṁse ito kappe,",
  "tha-ap156:2.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap156:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap156:2.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap156:3.1_0": "Ito ca navame kappe,",
  "tha-ap156:3.2_0": "rājā sattuttamo ahuṁ;",
  "tha-ap156:3.3_0": "Sattaratanasampanno,",
  "tha-ap156:3.4_0": "cakkavattī mahabbalo.",
  "tha-ap156:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap156:4.2_0": "…pe…",
  "tha-ap156:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap156:5.1_0": "Itthaṁ sudaṁ āyasmā kakkārupupphapūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap156:6.1_0": "Kakkārupupphapūjakattherassāpadānaṁ catutthaṁ."
}