{
  "kv5.4:0.1_0": "Kathāvatthu",
  "kv5.4:0.2_0": "Mahāpaṇṇāsaka",
  "kv5.4:0.3_0": "Pañcamavagga",
  "kv5.4:0.4_0": "Niyāmakathā",
  "kv5.4:1.1_0": "Aniyatassa niyāmagamanāya atthi ñāṇanti?",
  "kv5.4:1.2_0": "Āmantā.",
  "kv5.4:1.3_0": "Niyatassa aniyāmagamanāya atthi ñāṇanti?",
  "kv5.4:1.4_0": "Na hevaṁ vattabbe …pe….",
  "kv5.4:2.1_0": "Niyatassa aniyāmagamanāya natthi ñāṇanti?",
  "kv5.4:2.2_0": "Āmantā.",
  "kv5.4:2.3_0": "Aniyatassa niyāmagamanāya natthi ñāṇanti?",
  "kv5.4:2.4_0": "Na hevaṁ vattabbe …pe….",
  "kv5.4:3.1_0": "Aniyatassa niyāmagamanāya atthi ñāṇanti?",
  "kv5.4:3.2_0": "Āmantā.",
  "kv5.4:3.3_0": "Niyatassa niyāmagamanāya atthi ñāṇanti?",
  "kv5.4:3.4_0": "Na hevaṁ vattabbe …pe….",
  "kv5.4:4.1_0": "Niyatassa niyāmagamanāya natthi ñāṇanti?",
  "kv5.4:4.2_0": "Āmantā.",
  "kv5.4:4.3_0": "Aniyatassa niyāmagamanāya natthi ñāṇanti?",
  "kv5.4:4.4_0": "Na hevaṁ vattabbe …pe….",
  "kv5.4:5.1_0": "Aniyatassa niyāmagamanāya atthi ñāṇanti?",
  "kv5.4:5.2_0": "Āmantā.",
  "kv5.4:5.3_0": "Aniyatassa aniyāmagamanāya atthi ñāṇanti?",
  "kv5.4:5.4_0": "Na hevaṁ vattabbe …pe….",
  "kv5.4:6.1_0": "Aniyatassa aniyāmagamanāya natthi ñāṇanti?",
  "kv5.4:6.2_0": "Āmantā.",
  "kv5.4:6.3_0": "Aniyatassa niyāmagamanāya natthi ñāṇanti?",
  "kv5.4:6.4_0": "Na hevaṁ vattabbe …pe….",
  "kv5.4:7.1_0": "Aniyatassa niyāmagamanāya atthi ñāṇanti?",
  "kv5.4:7.2_0": "Āmantā.",
  "kv5.4:7.3_0": "Aniyatassa niyāmagamanāya atthi niyāmoti?",
  "kv5.4:7.4_0": "Na hevaṁ vattabbe …pe…",
  "kv5.4:8.1_0": "Aniyatassa niyāmagamanāya atthi ñāṇanti?",
  "kv5.4:8.2_0": "Āmantā.",
  "kv5.4:8.3_0": "Aniyatassa niyāmagamanāya atthi satipaṭṭhānā …",
  "kv5.4:8.4_0": "sammappadhānā …",
  "kv5.4:8.5_0": "iddhipādā …",
  "kv5.4:8.6_0": "indriyā …",
  "kv5.4:8.7_0": "balā …",
  "kv5.4:8.8_0": "bojjhaṅgāti?",
  "kv5.4:8.9_0": "Na hevaṁ vattabbe …pe….",
  "kv5.4:9.1_0": "Aniyatassa niyāmagamanāya natthi niyāmoti?",
  "kv5.4:9.2_0": "Āmantā.",
  "kv5.4:9.3_0": "Hañci aniyatassa niyāmagamanāya natthi niyāmo, no vata re vattabbe—",
  "kv5.4:9.4_0": "“aniyatassa niyāmagamanāya atthi ñāṇan”ti.",
  "kv5.4:10.1_0": "Aniyatassa niyāmagamanāya natthi satipaṭṭhānā …pe…",
  "kv5.4:10.2_0": "bojjhaṅgāti?",
  "kv5.4:10.3_0": "Āmantā.",
  "kv5.4:10.4_0": "Hañci aniyatassa niyāmagamanāya natthi bojjhaṅgā, no vata re vattabbe—",
  "kv5.4:10.5_0": "“aniyatassa niyāmagamanāya atthi ñāṇan”ti.",
  "kv5.4:11.1_0": "Aniyatassa niyāmagamanāya atthi ñāṇanti?",
  "kv5.4:11.2_0": "Āmantā.",
  "kv5.4:11.3_0": "Gotrabhuno puggalassa sotāpattimagge ñāṇaṁ atthīti?",
  "kv5.4:11.4_0": "Na hevaṁ vattabbe …pe…",
  "kv5.4:11.5_0": "sotāpattiphalasacchikiriyāya paṭipannassa puggalassa sotāpattiphale ñāṇaṁ atthīti?",
  "kv5.4:11.6_0": "Na hevaṁ vattabbe …pe…",
  "kv5.4:11.7_0": "arahattasacchikiriyāya paṭipannassa puggalassa arahatte ñāṇaṁ atthīti?",
  "kv5.4:11.8_0": "Na hevaṁ vattabbe …pe….",
  "kv5.4:12.1_0": "Na vattabbaṁ—",
  "kv5.4:12.2_0": "“aniyatassa niyāmagamanāya atthi ñāṇan”ti?",
  "kv5.4:12.3_0": "Āmantā.",
  "kv5.4:12.4_0": "Nanu bhagavā jānāti—",
  "kv5.4:12.5_0": "“ayaṁ puggalo sammattaniyāmaṁ okkamissati, bhabbo ayaṁ puggalo dhammaṁ abhisametun”ti?",
  "kv5.4:12.6_0": "Āmantā.",
  "kv5.4:12.7_0": "Hañci bhagavā jānāti—",
  "kv5.4:12.8_0": "“ayaṁ puggalo sammattaniyāmaṁ okkamissati, bhabbo ayaṁ puggalo dhammaṁ abhisametuṁ”, tena vata re vattabbe—",
  "kv5.4:12.9_0": "“aniyatassa niyāmagamanāya atthi ñāṇan”ti.",
  "kv5.4:12.10_0": "Niyāmakathā niṭṭhitā."
}