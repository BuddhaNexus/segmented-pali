{
  "iti77:0.1_0": "Itivuttaka 77",
  "iti77:0.2_0": "Tikanipāta",
  "iti77:0.3_0": "Tatiyavagga",
  "iti77:0.4_0": "Bhidurasutta",
  "iti77:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti77:2.1_0": "“Bhidurāyaṁ, bhikkhave, kāyo, viññāṇaṁ virāgadhammaṁ, sabbe upadhī aniccā dukkhā vipariṇāmadhammā”ti.",
  "iti77:2.2_0": "Etamatthaṁ bhagavā avoca.",
  "iti77:2.3_0": "Tatthetaṁ iti vuccati:",
  "iti77:3.1_0": "“Kāyañca bhiduraṁ ñatvā,",
  "iti77:3.2_0": "Viññāṇañca virāgunaṁ;",
  "iti77:3.3_0": "Upadhīsu bhayaṁ disvā,",
  "iti77:3.4_0": "Jātimaraṇamaccagā;",
  "iti77:3.5_0": "Sampatvā paramaṁ santiṁ,",
  "iti77:3.6_0": "Kālaṁ kaṅkhati bhāvitatto”ti.",
  "iti77:4.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti77:4.2_0": "Aṭṭhamaṁ."
}