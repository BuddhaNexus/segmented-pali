{
  "ja329:0.1_0": "Jātaka",
  "ja329:0.2_0": "Catukkanipāta",
  "ja329:0.3_0": "Kuṭidūsakavagga",
  "ja329:0.4_0": "9. Kāḷabāhujātaka",
  "ja329:1.1_0": "“Yaṁ annapānassa pure labhāma,",
  "ja329:1.2_0": "Taṁ dāni sākhamigameva gacchati;",
  "ja329:1.3_0": "Gacchāma dāni vanameva rādha,",
  "ja329:1.4_0": "Asakkatā casma dhanañjayāya”.",
  "ja329:2.1_0": "“Lābho alābho yaso ayaso ca,",
  "ja329:2.2_0": "Nindā pasaṁsā ca sukhañca dukkhaṁ;",
  "ja329:2.3_0": "Ete aniccā manujesu dhammā,",
  "ja329:2.4_0": "Mā soci kiṁ socasi poṭṭhapāda”.",
  "ja329:3.1_0": "“Addhā tuvaṁ paṇḍitakosi rādha,",
  "ja329:3.2_0": "Jānāsi atthāni anāgatāni;",
  "ja329:3.3_0": "Kathaṁ nu sākhāmigaṁ dakkhisāma,",
  "ja329:3.4_0": "Niddhāvitaṁ rājakulatova jammaṁ”.",
  "ja329:4.1_0": "“Cāleti kaṇṇaṁ bhakuṭiṁ karoti,",
  "ja329:4.2_0": "Muhuṁ muhuṁ bhāyayate kumāre;",
  "ja329:4.3_0": "Sayameva taṁ kāhati kāḷabāhu,",
  "ja329:4.4_0": "Yenārakā ṭhassati annapānā”ti.",
  "ja329:5.1_0": "Kāḷabāhujātakaṁ navamaṁ."
}