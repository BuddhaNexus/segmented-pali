{
  "tha-ap556:0.1_0": "Therāpadāna",
  "tha-ap556:0.2_0": "Yasavagga",
  "tha-ap556:0.3_0": "4 Kimilattheraapadāna",
  "tha-ap556:1.1_0": "“Nibbute kakusandhamhi,",
  "tha-ap556:1.2_0": "brāhmaṇamhi vusīmati;",
  "tha-ap556:1.3_0": "Gahetvā salalaṁ mālaṁ,",
  "tha-ap556:1.4_0": "maṇḍapaṁ kārayiṁ ahaṁ.",
  "tha-ap556:2.1_0": "Tāvatiṁsaṁ gato santo,",
  "tha-ap556:2.2_0": "labhimha byamhamuttamaṁ;",
  "tha-ap556:2.3_0": "Aññe devetirocāmi,",
  "tha-ap556:2.4_0": "puññakammassidaṁ phalaṁ.",
  "tha-ap556:3.1_0": "Divā vā yadi vā rattiṁ,",
  "tha-ap556:3.2_0": "caṅkamanto ṭhito cahaṁ;",
  "tha-ap556:3.3_0": "Channo salalapupphehi,",
  "tha-ap556:3.4_0": "puññakammassidaṁ phalaṁ.",
  "tha-ap556:4.1_0": "Imasmiṁyeva kappamhi,",
  "tha-ap556:4.2_0": "yaṁ buddhamabhipūjayiṁ;",
  "tha-ap556:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap556:4.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap556:5.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap556:5.2_0": "…pe…",
  "tha-ap556:5.3_0": "viharāmi anāsavo.",
  "tha-ap556:6.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap556:6.2_0": "…pe…",
  "tha-ap556:6.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap556:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap556:7.2_0": "…pe…",
  "tha-ap556:7.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap556:8.1_0": "Itthaṁ sudaṁ āyasmā kimilo thero imā gāthāyo abhāsitthāti.",
  "tha-ap556:9.1_0": "Kimilattherassāpadānaṁ catutthaṁ."
}