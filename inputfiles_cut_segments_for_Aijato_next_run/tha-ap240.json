{
  "tha-ap240:0.1_0": "Therāpadāna",
  "tha-ap240:0.2_0": "Udakāsanavagga",
  "tha-ap240:0.3_0": "8. Ambayāgadāyakattheraapadāna",
  "tha-ap240:1.1_0": "“Sake sippe apatthaddho,",
  "tha-ap240:1.2_0": "agamaṁ kānanaṁ ahaṁ;",
  "tha-ap240:1.3_0": "Sambuddhaṁ yantaṁ disvāna,",
  "tha-ap240:1.4_0": "ambayāgaṁ adāsahaṁ.",
  "tha-ap240:2.1_0": "Ekanavutito kappe,",
  "tha-ap240:2.2_0": "yaṁ dānamadadiṁ tadā;",
  "tha-ap240:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap240:2.4_0": "ambayāgassidaṁ phalaṁ.",
  "tha-ap240:3.1_0": "Paṭisambhidā catasso,",
  "tha-ap240:3.2_0": "…pe…",
  "tha-ap240:3.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap240:4.1_0": "Itthaṁ sudaṁ āyasmā ambayāgadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap240:5.1_0": "Ambayāgadāyakattherassāpadānaṁ aṭṭhamaṁ."
}