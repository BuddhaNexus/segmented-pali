{
  "cp19:0.1_0": "Cariyāpiṭaka",
  "cp19:0.2_0": "Hatthināgavagga",
  "cp19:0.3_0": "Sīlapāramī 9",
  "cp19:0.4_0": "Alīnasattucariya",
  "cp19:1.1_0": "“Pañcālaraṭṭhe nagaravare,",
  "cp19:1.2_0": "Kapilāyaṁ puruttame;",
  "cp19:1.3_0": "Rājā jayaddiso nāma,",
  "cp19:1.4_0": "Sīlaguṇamupāgato.",
  "cp19:2.1_0": "Tassa rañño ahaṁ putto,",
  "cp19:2.2_0": "Sutadhammo susīlavā;",
  "cp19:2.3_0": "Alīnasatto guṇavā,",
  "cp19:2.4_0": "Anurakkhaparijano sadā.",
  "cp19:3.1_0": "Pitā me migavaṁ gantvā,",
  "cp19:3.2_0": "porisādaṁ upāgami;",
  "cp19:3.3_0": "So me pitumaggahesi,",
  "cp19:3.4_0": "‘bhakkhosi mama mā cali’.",
  "cp19:4.1_0": "Tassa taṁ vacanaṁ sutvā,",
  "cp19:4.2_0": "bhīto tasitavedhito;",
  "cp19:4.3_0": "Ūrukkhambho ahu tassa,",
  "cp19:4.4_0": "disvāna porisādakaṁ.",
  "cp19:5.1_0": "Migavaṁ gahetvā muñcassu,",
  "cp19:5.2_0": "katvā āgamanaṁ puna;",
  "cp19:5.3_0": "Brāhmaṇassa dhanaṁ datvā,",
  "cp19:5.4_0": "pitā āmantayī mamaṁ.",
  "cp19:6.1_0": "‘Rajjaṁ putta paṭipajja,",
  "cp19:6.2_0": "mā pamajji puraṁ idaṁ;",
  "cp19:6.3_0": "Kataṁ me porisādena,",
  "cp19:6.4_0": "mama āgamanaṁ puna’.",
  "cp19:7.1_0": "Mātāpitū ca vanditvā,",
  "cp19:7.2_0": "nimminitvāna attanā;",
  "cp19:7.3_0": "Nikkhipitvā dhanuṁ khaggaṁ,",
  "cp19:7.4_0": "porisādaṁ upāgamiṁ.",
  "cp19:8.1_0": "Sasatthahatthūpagataṁ,",
  "cp19:8.2_0": "kadāci so tasissati;",
  "cp19:8.3_0": "Tena bhijjissati sīlaṁ,",
  "cp19:8.4_0": "parittāsaṁ kate mayi.",
  "cp19:9.1_0": "Sīlakhaṇḍabhayā mayhaṁ,",
  "cp19:9.2_0": "tassa dessaṁ na byāhariṁ;",
  "cp19:9.3_0": "Mettacitto hitavādī,",
  "cp19:9.4_0": "idaṁ vacanamabraviṁ.",
  "cp19:10.1_0": "‘Ujjālehi mahāaggiṁ,",
  "cp19:10.2_0": "papatissāmi rukkhato;",
  "cp19:10.3_0": "Tvaṁ pakkakālamaññāya,",
  "cp19:10.4_0": "bhakkhaya maṁ pitāmaha’.",
  "cp19:11.1_0": "Iti sīlavataṁ hetu,",
  "cp19:11.2_0": "nārakkhiṁ mama jīvitaṁ;",
  "cp19:11.3_0": "Pabbājesiṁ cahaṁ tassa,",
  "cp19:11.4_0": "sadā pāṇātipātikan”ti.",
  "cp19:12.1_0": "Alīnasattucariyaṁ navamaṁ."
}