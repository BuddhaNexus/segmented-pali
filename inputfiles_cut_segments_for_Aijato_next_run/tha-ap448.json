{
  "tha-ap448:0.1_0": "Therāpadāna",
  "tha-ap448:0.2_0": "Vibhītakavagga",
  "tha-ap448:0.3_0": "6. Ambāṭakiyattheraapadāna",
  "tha-ap448:1.1_0": "“Supupphitaṁ sālavanaṁ,",
  "tha-ap448:1.2_0": "ogayha vessabhū muni;",
  "tha-ap448:1.3_0": "Nisīdi giriduggesu,",
  "tha-ap448:1.4_0": "abhijātova kesarī.",
  "tha-ap448:2.1_0": "Pasannacitto sumano,",
  "tha-ap448:2.2_0": "ambāṭakamapūjayiṁ;",
  "tha-ap448:2.3_0": "Puññakkhettaṁ anuttaraṁ,",
  "tha-ap448:2.4_0": "pasanno sehi pāṇibhi.",
  "tha-ap448:3.1_0": "Ekatiṁse ito kappe,",
  "tha-ap448:3.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap448:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap448:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap448:4.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap448:4.2_0": "…pe…",
  "tha-ap448:4.3_0": "viharāmi anāsavo.",
  "tha-ap448:5.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap448:5.2_0": "…pe…",
  "tha-ap448:5.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap448:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap448:6.2_0": "…pe…",
  "tha-ap448:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap448:7.1_0": "Itthaṁ sudaṁ āyasmā ambāṭakiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap448:8.1_0": "Ambāṭakiyattherassāpadānaṁ chaṭṭhaṁ."
}