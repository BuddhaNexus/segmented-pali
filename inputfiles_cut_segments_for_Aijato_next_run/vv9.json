{
  "vv9:0.1_0": "Vimānavatthu",
  "vv9:0.2_0": "Itthivimāna",
  "vv9:0.3_0": "Pīṭhavagga",
  "vv9:0.4_0": "9. Dīpavimānavatthu",
  "vv9:1.1_0": "“Abhikkantena vaṇṇena,",
  "vv9:1.2_0": "yā tvaṁ tiṭṭhasi devate;",
  "vv9:1.3_0": "Obhāsentī disā sabbā,",
  "vv9:1.4_0": "osadhī viya tārakā.",
  "vv9:2.1_0": "Kena tetādiso vaṇṇo,",
  "vv9:2.2_0": "kena te idha mijjhati;",
  "vv9:2.3_0": "Uppajjanti ca te bhogā,",
  "vv9:2.4_0": "ye keci manaso piyā.",
  "vv9:3.1_0": "Kena tvaṁ vimalobhāsā,",
  "vv9:3.2_0": "atirocasi devatā;",
  "vv9:3.3_0": "Kena te sabbagattehi,",
  "vv9:3.4_0": "sabbā obhāsate disā.",
  "vv9:4.1_0": "Pucchāmi taṁ devi mahānubhāve,",
  "vv9:4.2_0": "Manussabhūtā kimakāsi puññaṁ;",
  "vv9:4.3_0": "Kenāsi evaṁ jalitānubhāvā,",
  "vv9:4.4_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv9:5.1_0": "Sā devatā attamanā,",
  "vv9:5.2_0": "moggallānena pucchitā;",
  "vv9:5.3_0": "Pañhaṁ puṭṭhā viyākāsi,",
  "vv9:5.4_0": "yassa kammassidaṁ phalaṁ.",
  "vv9:6.1_0": "“Ahaṁ manussesu manussabhūtā,",
  "vv9:6.2_0": "Purimāya jātiyā manussaloke;",
  "vv9:6.3_0": "Tamandhakāramhi timīsikāyaṁ,",
  "vv9:6.4_0": "Padīpakālamhi adāsi dīpaṁ.",
  "vv9:7.1_0": "Yo andhakāramhi timīsikāyaṁ,",
  "vv9:7.2_0": "Padīpakālamhi dadāti dīpaṁ;",
  "vv9:7.3_0": "Upapajjati jotirasaṁ vimānaṁ,",
  "vv9:7.4_0": "Pahūtamalyaṁ bahupuṇḍarīkaṁ.",
  "vv9:8.1_0": "Tena metādiso vaṇṇo,",
  "vv9:8.2_0": "tena me idha mijjhati;",
  "vv9:8.3_0": "Uppajjanti ca me bhogā,",
  "vv9:8.4_0": "ye keci manaso piyā.",
  "vv9:9.1_0": "Tenāhaṁ vimalobhāsā,",
  "vv9:9.2_0": "atirocāmi devatā;",
  "vv9:9.3_0": "Tena me sabbagattehi,",
  "vv9:9.4_0": "sabbā obhāsate disā.",
  "vv9:10.1_0": "Akkhāmi te bhikkhu mahānubhāva,",
  "vv9:10.2_0": "Manussabhūtā yamakāsi puññaṁ;",
  "vv9:10.3_0": "Tenamhi evaṁ jalitānubhāvā,",
  "vv9:10.4_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv9:11.1_0": "Dīpavimānaṁ navamaṁ."
}