{
  "snp5.19:0.1_0": "Sutta Nipāta 5.19",
  "snp5.19:0.2_0": "Pārāyanānugītigāthā",
  "snp5.19:1.1_0": "“Pārāyanamanugāyissaṁ,",
  "snp5.19:1.2_0": "(iccāyasmā piṅgiyo)",
  "snp5.19:1.3_0": "Yathāddakkhi tathākkhāsi;",
  "snp5.19:1.4_0": "Vimalo bhūrimedhaso,",
  "snp5.19:1.5_0": "Nikkāmo nibbano nāgo;",
  "snp5.19:1.6_0": "Kissa hetu musā bhaṇe.",
  "snp5.19:2.1_0": "Pahīnamalamohassa,",
  "snp5.19:2.2_0": "mānamakkhappahāyino;",
  "snp5.19:2.3_0": "Handāhaṁ kittayissāmi,",
  "snp5.19:2.4_0": "giraṁ vaṇṇūpasañhitaṁ.",
  "snp5.19:3.1_0": "Tamonudo buddho samantacakkhu,",
  "snp5.19:3.2_0": "Lokantagū sabbabhavātivatto;",
  "snp5.19:3.3_0": "Anāsavo sabbadukkhappahīno,",
  "snp5.19:3.4_0": "Saccavhayo brahme upāsito me.",
  "snp5.19:4.1_0": "Dijo yathā kubbanakaṁ pahāya,",
  "snp5.19:4.2_0": "Bahupphalaṁ kānanamāvaseyya;",
  "snp5.19:4.3_0": "Evampahaṁ appadasse pahāya,",
  "snp5.19:4.4_0": "Mahodadhiṁ haṁsoriva ajjhapatto.",
  "snp5.19:5.1_0": "Ye me pubbe viyākaṁsu,",
  "snp5.19:5.2_0": "Huraṁ gotamasāsanā;",
  "snp5.19:5.3_0": "Iccāsi iti bhavissati,",
  "snp5.19:5.4_0": "Sabbaṁ taṁ itihītihaṁ;",
  "snp5.19:5.5_0": "Sabbaṁ taṁ takkavaḍḍhanaṁ.",
  "snp5.19:6.1_0": "Eko tamanudāsino,",
  "snp5.19:6.2_0": "jutimā so pabhaṅkaro;",
  "snp5.19:6.3_0": "Gotamo bhūripaññāṇo,",
  "snp5.19:6.4_0": "gotamo bhūrimedhaso.",
  "snp5.19:7.1_0": "Yo me dhammamadesesi,",
  "snp5.19:7.2_0": "Sandiṭṭhikamakālikaṁ;",
  "snp5.19:7.3_0": "Taṇhakkhayamanītikaṁ,",
  "snp5.19:7.4_0": "Yassa natthi upamā kvaci”.",
  "snp5.19:8.1_0": "“Kiṁ nu tamhā vippavasasi,",
  "snp5.19:8.2_0": "muhuttamapi piṅgiya;",
  "snp5.19:8.3_0": "Gotamā bhūripaññāṇā,",
  "snp5.19:8.4_0": "gotamā bhūrimedhasā.",
  "snp5.19:9.1_0": "Yo te dhammamadesesi,",
  "snp5.19:9.2_0": "Sandiṭṭhikamakālikaṁ;",
  "snp5.19:9.3_0": "Taṇhakkhayamanītikaṁ,",
  "snp5.19:9.4_0": "Yassa natthi upamā kvaci”.",
  "snp5.19:10.1_0": "“Nāhaṁ tamhā vippavasāmi,",
  "snp5.19:10.2_0": "Muhuttamapi brāhmaṇa;",
  "snp5.19:10.3_0": "Gotamā bhūripaññāṇā,",
  "snp5.19:10.4_0": "Gotamā bhūrimedhasā.",
  "snp5.19:11.1_0": "Yo me dhammamadesesi,",
  "snp5.19:11.2_0": "Sandiṭṭhikamakālikaṁ;",
  "snp5.19:11.3_0": "Taṇhakkhayamanītikaṁ,",
  "snp5.19:11.4_0": "Yassa natthi upamā kvaci.",
  "snp5.19:12.1_0": "Passāmi naṁ manasā cakkhunāva,",
  "snp5.19:12.2_0": "Rattindivaṁ brāhmaṇa appamatto;",
  "snp5.19:12.3_0": "Namassamāno vivasemi rattiṁ,",
  "snp5.19:12.4_0": "Teneva maññāmi avippavāsaṁ.",
  "snp5.19:13.1_0": "Saddhā ca pīti ca mano sati ca,",
  "snp5.19:13.2_0": "Nāpentime gotamasāsanamhā;",
  "snp5.19:13.3_0": "Yaṁ yaṁ disaṁ vajati bhūripañño,",
  "snp5.19:13.4_0": "Sa tena teneva natohamasmi.",
  "snp5.19:14.1_0": "Jiṇṇassa me dubbalathāmakassa,",
  "snp5.19:14.2_0": "Teneva kāyo na paleti tattha;",
  "snp5.19:14.3_0": "Saṅkappayantāya vajāmi niccaṁ,",
  "snp5.19:14.4_0": "Mano hi me brāhmaṇa tena yutto.",
  "snp5.19:15.1_0": "Paṅke sayāno pariphandamāno,",
  "snp5.19:15.2_0": "Dīpā dīpaṁ upaplaviṁ;",
  "snp5.19:15.3_0": "Athaddasāsiṁ sambuddhaṁ,",
  "snp5.19:15.4_0": "Oghatiṇṇamanāsavaṁ”.",
  "snp5.19:16.1_0": "“Yathā ahū vakkali muttasaddho,",
  "snp5.19:16.2_0": "Bhadrāvudho āḷavigotamo ca;",
  "snp5.19:16.3_0": "Evamevaṁ tvampi pamuñcassu saddhaṁ,",
  "snp5.19:16.4_0": "Gamissasi tvaṁ piṅgiya maccudheyyassa pāraṁ”.",
  "snp5.19:17.1_0": "“Esa bhiyyo pasīdāmi,",
  "snp5.19:17.2_0": "Sutvāna munino vaco;",
  "snp5.19:17.3_0": "Vivaṭṭacchado sambuddho,",
  "snp5.19:17.4_0": "Akhilo paṭibhānavā.",
  "snp5.19:18.1_0": "Adhideve abhiññāya,",
  "snp5.19:18.2_0": "Sabbaṁ vedi varovaraṁ;",
  "snp5.19:18.3_0": "Pañhānantakaro satthā,",
  "snp5.19:18.4_0": "Kaṅkhīnaṁ paṭijānataṁ.",
  "snp5.19:19.1_0": "Asaṁhīraṁ asaṅkuppaṁ,",
  "snp5.19:19.2_0": "Yassa natthi upamā kvaci;",
  "snp5.19:19.3_0": "Addhā gamissāmi na mettha kaṅkhā,",
  "snp5.19:19.4_0": "Evaṁ maṁ dhārehi adhimuttacittan”ti.",
  "snp5.19:19.5_0": "Pārāyanavaggo pañcamo.",
  "snp5.19:19.6_0": "Suttuddānaṁ",
  "snp5.19:20.1_0": "Urago dhaniyopi ca,",
  "snp5.19:20.2_0": "Khaggavisāṇo kasi ca;",
  "snp5.19:20.3_0": "Cundo bhavo punadeva,",
  "snp5.19:20.4_0": "Vasalo ca karaṇīyañca;",
  "snp5.19:20.5_0": "Hemavato atha yakkho,",
  "snp5.19:20.6_0": "Vijayasuttaṁ munisuttavaranti.",
  "snp5.19:21.1_0": "Paṭhamakaṭṭhavaro varavaggo,",
  "snp5.19:21.2_0": "Dvādasasuttadharo suvibhatto;",
  "snp5.19:21.3_0": "Desito cakkhumatā vimalena,",
  "snp5.19:21.4_0": "Suyyati vaggavaro uragoti.",
  "snp5.19:22.1_0": "Ratanāmagandho hirimaṅgalanāmo,",
  "snp5.19:22.2_0": "Sucilomakapilo ca brāhmaṇadhammo;",
  "snp5.19:22.3_0": "Nāvā kiṁsīlauṭṭhahano ca,",
  "snp5.19:22.4_0": "Rāhulo ca punapi vaṅgīso.",
  "snp5.19:23.1_0": "Sammāparibbājanīyopi cettha,",
  "snp5.19:23.2_0": "Dhammikasuttavaro suvibhatto;",
  "snp5.19:23.3_0": "Cuddasasuttadharo dutiyamhi,",
  "snp5.19:23.4_0": "Cūḷakavaggavaroti tamāhu.",
  "snp5.19:24.1_0": "Pabbajjapadhānasubhāsitanāmo,",
  "snp5.19:24.2_0": "Pūraḷāso punadeva māgho ca;",
  "snp5.19:24.3_0": "Sabhiyaṁ keṇiyameva sallanāmo,",
  "snp5.19:24.4_0": "Vāseṭṭhavaro kālikopi ca.",
  "snp5.19:25.1_0": "Nālakasuttavaro suvibhatto,",
  "snp5.19:25.2_0": "Taṁ anupassī tathā punadeva;",
  "snp5.19:25.3_0": "Dvādasasuttadharo tatiyamhi,",
  "snp5.19:25.4_0": "Suyyati vaggavaro mahānāmo.",
  "snp5.19:26.1_0": "Kāmaguhaṭṭhakaduṭṭhakanāmā,",
  "snp5.19:26.2_0": "Suddhavaro paramaṭṭhakanāmo;",
  "snp5.19:26.3_0": "Jarā mettiyavaro suvibhatto,",
  "snp5.19:26.4_0": "Pasūramāgaṇḍiyā purābhedo.",
  "snp5.19:27.1_0": "Kalahavivādo ubho viyuhā ca,",
  "snp5.19:27.2_0": "Tuvaṭakaattadaṇḍasāriputtā;",
  "snp5.19:27.3_0": "Soḷasasuttadharo catutthamhi,",
  "snp5.19:27.4_0": "Aṭṭhakavaggavaroti tamāhu.",
  "snp5.19:28.1_0": "Magadhe janapade ramaṇīye,",
  "snp5.19:28.2_0": "Desavare katapuññanivese;",
  "snp5.19:28.3_0": "Pāsāṇakacetiyavare suvibhatte,",
  "snp5.19:28.4_0": "Vasi bhagavā gaṇaseṭṭho.",
  "snp5.19:29.1_0": "Ubhayavāsamāgatiyamhi,",
  "snp5.19:29.2_0": "Dvādasayojaniyā parisāya;",
  "snp5.19:29.3_0": "Soḷasabrāhmaṇānaṁ kira puṭṭho,",
  "snp5.19:29.4_0": "Pucchāya soḷasapañhakammiyā;",
  "snp5.19:29.5_0": "Nippakāsayi dhammamadāsi.",
  "snp5.19:30.1_0": "Atthapakāsakabyañjanapuṇṇaṁ,",
  "snp5.19:30.2_0": "Dhammamadesesi parakhemajaniyaṁ;",
  "snp5.19:30.3_0": "Lokahitāya jino dvipadaggo,",
  "snp5.19:30.4_0": "Suttavaraṁ bahudhammavicitraṁ;",
  "snp5.19:30.5_0": "Sabbakilesapamocanahetuṁ,",
  "snp5.19:30.6_0": "Desayi suttavaraṁ dvipadaggo.",
  "snp5.19:31.1_0": "Byañjanamatthapadaṁ samayuttaṁ,",
  "snp5.19:31.2_0": "Akkharasaññitaopamagāḷhaṁ;",
  "snp5.19:31.3_0": "Lokavicāraṇañāṇapabhaggaṁ,",
  "snp5.19:31.4_0": "Desayi suttavaraṁ dvipadaggo.",
  "snp5.19:32.1_0": "Rāgamale amalaṁ vimalaggaṁ,",
  "snp5.19:32.2_0": "Dosamale amalaṁ vimalaggaṁ;",
  "snp5.19:32.3_0": "Mohamale amalaṁ vimalaggaṁ,",
  "snp5.19:32.4_0": "Lokavicāraṇañāṇapabhaggaṁ;",
  "snp5.19:32.5_0": "Desayi suttavaraṁ dvipadaggo.",
  "snp5.19:33.1_0": "Klesamale amalaṁ vimalaggaṁ,",
  "snp5.19:33.2_0": "Duccaritamale amalaṁ vimalaggaṁ;",
  "snp5.19:33.3_0": "Lokavicāraṇañāṇapabhaggaṁ,",
  "snp5.19:33.4_0": "Desayi suttavaraṁ dvipadaggo.",
  "snp5.19:34.1_0": "Āsavabandhanayogākilesaṁ,",
  "snp5.19:34.2_0": "Nīvaraṇāni ca tīṇi malāni;",
  "snp5.19:34.3_0": "Tassa kilesapamocanahetuṁ,",
  "snp5.19:34.4_0": "Desayi suttavaraṁ dvipadaggo.",
  "snp5.19:35.1_0": "Nimmalasabbakilesapanūdaṁ,",
  "snp5.19:35.2_0": "Rāgavirāgamanejamasokaṁ;",
  "snp5.19:35.3_0": "Santapaṇītasududdasadhammaṁ,",
  "snp5.19:35.4_0": "Desayi suttavaraṁ dvipadaggo.",
  "snp5.19:36.1_0": "Rāgañca dosakamabhañjitasantaṁ,",
  "snp5.19:36.2_0": "Yonicatuggatipañcaviññāṇaṁ;",
  "snp5.19:36.3_0": "Taṇhāratacchadanatāṇalatāpamokkhaṁ,",
  "snp5.19:36.4_0": "Desayi suttavaraṁ dvipadaggo.",
  "snp5.19:37.1_0": "Gambhīraduddasasaṇhanipuṇaṁ,",
  "snp5.19:37.2_0": "Paṇḍitavedaniyaṁ nipuṇatthaṁ;",
  "snp5.19:37.3_0": "Lokavicāraṇañāṇapabhaggaṁ,",
  "snp5.19:37.4_0": "Desayi suttavaraṁ dvipadaggo.",
  "snp5.19:38.1_0": "Navaṅgakusumamālagīveyyaṁ,",
  "snp5.19:38.2_0": "Indriyajhānavimokkhavibhattaṁ;",
  "snp5.19:38.3_0": "Aṭṭhaṅgamaggadharaṁ varayānaṁ,",
  "snp5.19:38.4_0": "Desayi suttavaraṁ dvipadaggo.",
  "snp5.19:39.1_0": "Somupamaṁ vimalaṁ parisuddhaṁ,",
  "snp5.19:39.2_0": "Aṇṇavamūpamaratanasucittaṁ;",
  "snp5.19:39.3_0": "Pupphasamaṁ ravimūpamatejaṁ,",
  "snp5.19:39.4_0": "Desayi suttavaraṁ dvipadaggo.",
  "snp5.19:40.1_0": "Khemasivaṁ sukhasītalasantaṁ,",
  "snp5.19:40.2_0": "Maccutatāṇaparaṁ paramatthaṁ;",
  "snp5.19:40.3_0": "Tassa sunibbutadassanahetuṁ,",
  "snp5.19:40.4_0": "Desayi suttavaraṁ dvipadaggoti.",
  "snp5.19:40.5_0": "Suttanipātapāḷi samattā."
}