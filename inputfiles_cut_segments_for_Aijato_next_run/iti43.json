{
  "iti43:0.1_0": "Itivuttaka 43",
  "iti43:0.2_0": "Dukanipāta",
  "iti43:0.3_0": "Dutiyavagga",
  "iti43:0.4_0": "Ajātasutta",
  "iti43:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti43:2.1_0": "“Atthi, bhikkhave, ajātaṁ abhūtaṁ akataṁ asaṅkhataṁ.",
  "iti43:2.2_0": "No cetaṁ, bhikkhave, abhavissa ajātaṁ abhūtaṁ akataṁ asaṅkhataṁ, nayidha jātassa bhūtassa katassa saṅkhatassa nissaraṇaṁ paññāyetha.",
  "iti43:2.3_0": "Yasmā ca kho, bhikkhave, atthi ajātaṁ abhūtaṁ akataṁ asaṅkhataṁ, tasmā jātassa bhūtassa katassa saṅkhatassa nissaraṇaṁ paññāyatī”ti.",
  "iti43:2.4_0": "Etamatthaṁ bhagavā avoca.",
  "iti43:2.5_0": "Tatthetaṁ iti vuccati:",
  "iti43:3.1_0": "“Jātaṁ bhūtaṁ samuppannaṁ,",
  "iti43:3.2_0": "kataṁ saṅkhatamaddhuvaṁ;",
  "iti43:3.3_0": "Jarāmaraṇasaṅghāṭaṁ,",
  "iti43:3.4_0": "roganīḷaṁ pabhaṅguraṁ.",
  "iti43:4.1_0": "Āhāranettippabhavaṁ,",
  "iti43:4.2_0": "nālaṁ tadabhinandituṁ;",
  "iti43:4.3_0": "Tassa nissaraṇaṁ santaṁ,",
  "iti43:4.4_0": "atakkāvacaraṁ dhuvaṁ.",
  "iti43:5.1_0": "Ajātaṁ asamuppannaṁ,",
  "iti43:5.2_0": "asokaṁ virajaṁ padaṁ;",
  "iti43:5.3_0": "Nirodho dukkhadhammānaṁ,",
  "iti43:5.4_0": "saṅkhārūpasamo sukho”ti.",
  "iti43:6.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti43:6.2_0": "Chaṭṭhaṁ."
}