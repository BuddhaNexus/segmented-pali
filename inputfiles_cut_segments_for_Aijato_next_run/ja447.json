{
  "ja447:0.1_0": "Jātaka",
  "ja447:0.2_0": "Dasakanipāta",
  "ja447:0.3_0": "Catudvāravagga",
  "ja447:0.4_0": "9. Mahādhammapālajātaka",
  "ja447:1.1_0": "“Kiṁ te vataṁ kiṁ pana brahmacariyaṁ,",
  "ja447:1.2_0": "Kissa suciṇṇassa ayaṁ vipāko;",
  "ja447:1.3_0": "Akkhāhi me brāhmaṇa etamatthaṁ,",
  "ja447:1.4_0": "Kasmā nu tumhaṁ daharā na miyyare”.",
  "ja447:2.1_0": "“Dhammaṁ carāma na musā bhaṇāma,",
  "ja447:2.2_0": "Pāpāni kammāni parivajjayāma;",
  "ja447:2.3_0": "Anariyaṁ parivajjemu sabbaṁ,",
  "ja447:2.4_0": "Tasmā hi amhaṁ daharā na miyyare.",
  "ja447:3.1_0": "Suṇoma dhammaṁ asataṁ satañca,",
  "ja447:3.2_0": "Na cāpi dhammaṁ asataṁ rocayāma;",
  "ja447:3.3_0": "Hitvā asante na jahāma sante,",
  "ja447:3.4_0": "Tasmā hi amhaṁ daharā na miyyare.",
  "ja447:4.1_0": "Pubbeva dānā sumanā bhavāma,",
  "ja447:4.2_0": "Dadampi ve attamanā bhavāma;",
  "ja447:4.3_0": "Datvāpi ve nānutappāma pacchā,",
  "ja447:4.4_0": "Tasmā hi amhaṁ daharā na miyyare.",
  "ja447:5.1_0": "Samaṇe mayaṁ brāhmaṇe addhike ca,",
  "ja447:5.2_0": "Vanibbake yācanake dalidde;",
  "ja447:5.3_0": "Annena pānena abhitappayāma,",
  "ja447:5.4_0": "Tasmā hi amhaṁ daharā na miyyare.",
  "ja447:6.1_0": "Mayañca bhariyaṁ nātikkamāma,",
  "ja447:6.2_0": "Amhe ca bhariyā nātikkamanti;",
  "ja447:6.3_0": "Aññatra tāhi brahmacariyaṁ carāma,",
  "ja447:6.4_0": "Tasmā hi amhaṁ daharā na miyyare.",
  "ja447:7.1_0": "Pāṇātipātā viramāma sabbe,",
  "ja447:7.2_0": "Loke adinnaṁ parivajjayāma;",
  "ja447:7.3_0": "Amajjapā nopi musā bhaṇāma,",
  "ja447:7.4_0": "Tasmā hi amhaṁ daharā na miyyare.",
  "ja447:8.1_0": "Etāsu ve jāyare suttamāsu,",
  "ja447:8.2_0": "Medhāvino honti pahūtapaññā;",
  "ja447:8.3_0": "Bahussutā vedaguno ca honti,",
  "ja447:8.4_0": "Tasmā hi amhaṁ daharā na miyyare.",
  "ja447:9.1_0": "Mātā pitā ca bhaginī bhātaro ca,",
  "ja447:9.2_0": "Puttā ca dārā ca mayañca sabbe;",
  "ja447:9.3_0": "Dhammaṁ carāma paralokahetu,",
  "ja447:9.4_0": "Tasmā hi amhaṁ daharā na miyyare.",
  "ja447:10.1_0": "Dāsā ca dāsyo anujīvino ca,",
  "ja447:10.2_0": "Paricārakā kammakarā ca sabbe;",
  "ja447:10.3_0": "Dhammaṁ caranti paralokahetu,",
  "ja447:10.4_0": "Tasmā hi amhaṁ daharā na miyyare”.",
  "ja447:11.1_0": "“Dhammo have rakkhati dhammacāriṁ,",
  "ja447:11.2_0": "Dhammo suciṇṇo sukhamāvahāti;",
  "ja447:11.3_0": "Esānisaṁso dhamme suciṇṇe,",
  "ja447:11.4_0": "Na duggatiṁ gacchati dhammacārī.",
  "ja447:12.1_0": "Dhammo have rakkhati dhammacāriṁ,",
  "ja447:12.2_0": "Chattaṁ mahantaṁ viya vassakāle;",
  "ja447:12.3_0": "Dhammena gutto mama dhammapālo,",
  "ja447:12.4_0": "Aññassa aṭṭhīni sukhī kumāro”ti.",
  "ja447:13.1_0": "Mahādhammapālajātakaṁ navamaṁ."
}