{
  "ja356:0.1_0": "Jātaka",
  "ja356:0.2_0": "Pañcakanipāta",
  "ja356:0.3_0": "Maṇikuṇḍalavagga",
  "ja356:0.4_0": "6. Koraṇḍiyajātaka",
  "ja356:1.1_0": "“Eko araññe girikandarāyaṁ,",
  "ja356:1.2_0": "Paggayha paggayha silaṁ pavecchasi;",
  "ja356:1.3_0": "Punappunaṁ santaramānarūpo,",
  "ja356:1.4_0": "Koraṇḍiya ko nu tava yidhattho”.",
  "ja356:2.1_0": "“Ahañhimaṁ sāgara sevitantaṁ,",
  "ja356:2.2_0": "Samaṁ karissāmi yathāpi pāṇi;",
  "ja356:2.3_0": "Vikiriya sānūni ca pabbatāni ca,",
  "ja356:2.4_0": "Tasmā silaṁ dariyā pakkhipāmi”.",
  "ja356:3.1_0": "“Nayimaṁ mahiṁ arahati pāṇikappaṁ,",
  "ja356:3.2_0": "Samaṁ manusso karaṇāya meko;",
  "ja356:3.3_0": "Maññāmimaññeva dariṁ jigīsaṁ,",
  "ja356:3.4_0": "Koraṇḍiya hāhasi jīvalokaṁ”.",
  "ja356:4.1_0": "“Sace ahaṁ bhūtadharaṁ na sakkā,",
  "ja356:4.2_0": "Samaṁ manusso karaṇāya meko;",
  "ja356:4.3_0": "Evameva tvaṁ brahme ime manusse,",
  "ja356:4.4_0": "Nānādiṭṭhike nānayissasi te”.",
  "ja356:5.1_0": "“Saṅkhittarūpena bhavaṁ mamatthaṁ,",
  "ja356:5.2_0": "Akkhāsi koraṇḍiya evametaṁ;",
  "ja356:5.3_0": "Yathā na sakkā pathavī samāyaṁ,",
  "ja356:5.4_0": "Kattuṁ manussena tathā manussā”ti.",
  "ja356:6.1_0": "Koraṇḍiyajātakaṁ chaṭṭhaṁ."
}