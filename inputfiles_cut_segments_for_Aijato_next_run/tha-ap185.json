{
  "tha-ap185:0.1_0": "Therāpadāna",
  "tha-ap185:0.2_0": "Kuṭajapupphiyavagga",
  "tha-ap185:0.3_0": "3 Koṭumbariyattheraapadāna",
  "tha-ap185:1.1_0": "“Kaṇikāraṁva jotantaṁ,",
  "tha-ap185:1.2_0": "nisinnaṁ pabbatantare;",
  "tha-ap185:1.3_0": "Appameyyaṁva udadhiṁ,",
  "tha-ap185:1.4_0": "vitthataṁ dharaṇiṁ yathā.",
  "tha-ap185:2.1_0": "Pūjitaṁ devasaṅghena,",
  "tha-ap185:2.2_0": "nisabhājāniyaṁ yathā;",
  "tha-ap185:2.3_0": "Haṭṭho haṭṭhena cittena,",
  "tha-ap185:2.4_0": "upāgacchiṁ naruttamaṁ.",
  "tha-ap185:3.1_0": "Sattapupphāni paggayha,",
  "tha-ap185:3.2_0": "koṭumbarasamākulaṁ;",
  "tha-ap185:3.3_0": "Buddhassa abhiropesiṁ,",
  "tha-ap185:3.4_0": "sikhino lokabandhuno.",
  "tha-ap185:4.1_0": "Ekattiṁse ito kappe,",
  "tha-ap185:4.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap185:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap185:4.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap185:5.1_0": "Ito vīsatikappamhi,",
  "tha-ap185:5.2_0": "mahānelasanāmako;",
  "tha-ap185:5.3_0": "Eko āsi mahātejo,",
  "tha-ap185:5.4_0": "cakkavattī mahabbalo.",
  "tha-ap185:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap185:6.2_0": "…pe…",
  "tha-ap185:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap185:7.1_0": "Itthaṁ sudaṁ āyasmā koṭumbariyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap185:8.1_0": "Koṭumbariyattherassāpadānaṁ tatiyaṁ."
}