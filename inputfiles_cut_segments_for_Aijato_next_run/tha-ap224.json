{
  "tha-ap224:0.1_0": "Therāpadāna",
  "tha-ap224:0.2_0": "Ālambaṇadāyakavagga",
  "tha-ap224:0.3_0": "2. Ajinadāyakattheraapadāna",
  "tha-ap224:1.1_0": "“Ekattiṁse ito kappe,",
  "tha-ap224:1.2_0": "gaṇasatthārako ahaṁ;",
  "tha-ap224:1.3_0": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap224:1.4_0": "āhutīnaṁ paṭiggahaṁ.",
  "tha-ap224:2.1_0": "Cammakhaṇḍaṁ mayā dinnaṁ,",
  "tha-ap224:2.2_0": "sikhino lokabandhuno;",
  "tha-ap224:2.3_0": "Tena kammena dvipadinda,",
  "tha-ap224:2.4_0": "lokajeṭṭha narāsabha.",
  "tha-ap224:3.1_0": "Sampattiṁ anubhotvāna,",
  "tha-ap224:3.2_0": "kilese jhāpayiṁ ahaṁ;",
  "tha-ap224:3.3_0": "Dhāremi antimaṁ dehaṁ,",
  "tha-ap224:3.4_0": "sammāsambuddhasāsane.",
  "tha-ap224:4.1_0": "Ekattiṁse ito kappe,",
  "tha-ap224:4.2_0": "ajinaṁ yaṁ adāsahaṁ;",
  "tha-ap224:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap224:4.4_0": "ajinassa idaṁ phalaṁ.",
  "tha-ap224:5.1_0": "Ito pañcamake kappe,",
  "tha-ap224:5.2_0": "rājā āsiṁ sudāyako;",
  "tha-ap224:5.3_0": "Sattaratanasampanno,",
  "tha-ap224:5.4_0": "cakkavattī mahabbalo.",
  "tha-ap224:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap224:6.2_0": "…pe…",
  "tha-ap224:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap224:7.1_0": "Itthaṁ sudaṁ āyasmā ajinadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap224:8.1_0": "Ajinadāyakattherassāpadānaṁ dutiyaṁ."
}