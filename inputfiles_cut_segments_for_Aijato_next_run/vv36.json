{
  "vv36:0.1_0": "Vimānavatthu",
  "vv36:0.2_0": "Itthivimāna",
  "vv36:0.3_0": "Pāricchattakavagga",
  "vv36:0.4_0": "8. Mallikāvimānavatthu",
  "vv36:1.1_0": "“Pītavatthe pītadhaje,",
  "vv36:1.2_0": "pītālaṅkārabhūsite;",
  "vv36:1.3_0": "Pītantarāhi vaggūhi,",
  "vv36:1.4_0": "apiḷandhāva sobhasi.",
  "vv36:2.1_0": "Kā kambukāyūradhare,",
  "vv36:2.2_0": "kañcanāveḷabhūsite;",
  "vv36:2.3_0": "Hemajālakasañchanne,",
  "vv36:2.4_0": "nānāratanamālinī.",
  "vv36:3.1_0": "Sovaṇṇamayā lohitaṅgamayā ca,",
  "vv36:3.2_0": "Muttāmayā veḷuriyamayā ca;",
  "vv36:3.3_0": "Masāragallā sahalohitaṅgā,",
  "vv36:3.4_0": "Pārevatakkhīhi maṇīhi cittatā.",
  "vv36:4.1_0": "Koci koci ettha mayūrasussaro,",
  "vv36:4.2_0": "Haṁsassa rañño karavīkasussaro;",
  "vv36:4.3_0": "Tesaṁ saro suyyati vaggurūpo,",
  "vv36:4.4_0": "Pañcaṅgikaṁ tūriyamivappavāditaṁ.",
  "vv36:5.1_0": "Ratho ca te subho vaggu,",
  "vv36:5.2_0": "nānāratanacittito;",
  "vv36:5.3_0": "Nānāvaṇṇāhi dhātūhi,",
  "vv36:5.4_0": "suvibhattova sobhati.",
  "vv36:6.1_0": "Tasmiṁ rathe kañcanabimbavaṇṇe,",
  "vv36:6.2_0": "Yā tvaṁ ṭhitā bhāsasimaṁ padesaṁ;",
  "vv36:6.3_0": "Devate pucchitācikkha,",
  "vv36:6.4_0": "Kissa kammassidaṁ phalan”ti.",
  "vv36:7.1_0": "“Sovaṇṇajālaṁ maṇisoṇṇacittitaṁ,",
  "vv36:7.2_0": "Muttācitaṁ hemajālena channaṁ;",
  "vv36:7.3_0": "Parinibbute gotame appameyye,",
  "vv36:7.4_0": "Pasannacittā ahamābhiropayiṁ.",
  "vv36:8.1_0": "Tāhaṁ kammaṁ karitvāna,",
  "vv36:8.2_0": "kusalaṁ buddhavaṇṇitaṁ;",
  "vv36:8.3_0": "Apetasokā sukhitā,",
  "vv36:8.4_0": "sampamodāmanāmayā”ti.",
  "vv36:9.1_0": "Mallikāvimānaṁ aṭṭhamaṁ."
}