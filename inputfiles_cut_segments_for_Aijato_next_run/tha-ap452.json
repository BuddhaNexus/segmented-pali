{
  "tha-ap452:0.1_0": "Therāpadāna",
  "tha-ap452:0.2_0": "Vibhītakavagga",
  "tha-ap452:0.3_0": "10 Bodhigharadāyakattheraapadāna",
  "tha-ap452:1.1_0": "“Siddhatthassa bhagavato,",
  "tha-ap452:1.2_0": "dvipadindassa tādino;",
  "tha-ap452:1.3_0": "Pasannacitto sumano,",
  "tha-ap452:1.4_0": "bodhigharamakārayiṁ.",
  "tha-ap452:2.1_0": "Tusitaṁ upapannomhi,",
  "tha-ap452:2.2_0": "vasāmi ratane ghare;",
  "tha-ap452:2.3_0": "Na me sītaṁ vā uṇhaṁ vā,",
  "tha-ap452:2.4_0": "vāto gatte na samphuse.",
  "tha-ap452:3.1_0": "Pañcasaṭṭhimhito kappe,",
  "tha-ap452:3.2_0": "cakkavattī ahosahaṁ;",
  "tha-ap452:3.3_0": "Kāsikaṁ nāma nagaraṁ,",
  "tha-ap452:3.4_0": "vissakammena māpitaṁ.",
  "tha-ap452:4.1_0": "Dasayojanaāyāmaṁ,",
  "tha-ap452:4.2_0": "aṭṭhayojanavitthataṁ;",
  "tha-ap452:4.3_0": "Na tamhi nagare atthi,",
  "tha-ap452:4.4_0": "kaṭṭhaṁ vallī ca mattikā.",
  "tha-ap452:5.1_0": "Tiriyaṁ yojanaṁ āsi,",
  "tha-ap452:5.2_0": "addhayojanavitthataṁ;",
  "tha-ap452:5.3_0": "Maṅgalo nāma pāsādo,",
  "tha-ap452:5.4_0": "vissakammena māpito.",
  "tha-ap452:6.1_0": "Cullāsītisahassāni,",
  "tha-ap452:6.2_0": "thambhā soṇṇamayā ahuṁ;",
  "tha-ap452:6.3_0": "Maṇimayā ca niyyūhā,",
  "tha-ap452:6.4_0": "chadanaṁ rūpiyaṁ ahu.",
  "tha-ap452:7.1_0": "Sabbasoṇṇamayaṁ gharaṁ,",
  "tha-ap452:7.2_0": "vissakammena māpitaṁ;",
  "tha-ap452:7.3_0": "Ajjhāvutthaṁ mayā etaṁ,",
  "tha-ap452:7.4_0": "gharadānassidaṁ phalaṁ.",
  "tha-ap452:8.1_0": "Te sabbe anubhotvāna,",
  "tha-ap452:8.2_0": "devamānusake bhave;",
  "tha-ap452:8.3_0": "Ajjhapattomhi nibbānaṁ,",
  "tha-ap452:8.4_0": "santipadamanuttaraṁ.",
  "tha-ap452:9.1_0": "Tiṁsakappasahassamhi,",
  "tha-ap452:9.2_0": "bodhigharamakārayiṁ;",
  "tha-ap452:9.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap452:9.4_0": "gharadānassidaṁ phalaṁ.",
  "tha-ap452:10.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap452:10.2_0": "…pe…",
  "tha-ap452:10.3_0": "viharāmi anāsavo.",
  "tha-ap452:11.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap452:11.2_0": "…pe…",
  "tha-ap452:11.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap452:12.1_0": "Paṭisambhidā catasso,",
  "tha-ap452:12.2_0": "…pe…",
  "tha-ap452:12.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap452:13.1_0": "Itthaṁ sudaṁ āyasmā bodhigharadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap452:14.1_0": "Bodhigharadāyakattherassāpadānaṁ dasamaṁ.",
  "tha-ap452:15.1_0": "Vibhītakavaggo pañcacattālīsamo.",
  "tha-ap452:16.1_0": "Tassuddānaṁ",
  "tha-ap452:17.1_0": "Vibhītakī kolaphalī,",
  "tha-ap452:17.2_0": "billabhallātakappado;",
  "tha-ap452:17.3_0": "Uttalambaṭakī ceva,",
  "tha-ap452:17.4_0": "āsanī pādapīṭhako.",
  "tha-ap452:18.1_0": "Vediko bodhighariko,",
  "tha-ap452:18.2_0": "gāthāyo gaṇitāpi ca;",
  "tha-ap452:18.3_0": "Ekūnāsītikā sabbā,",
  "tha-ap452:18.4_0": "asmiṁ vagge pakittitā."
}