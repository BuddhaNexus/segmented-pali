{
  "dt2.8:0.1_0": "Dhātukathā",
  "dt2.8:0.2_0": "2 Niddesa",
  "dt2.8:0.3_0": "2.8. Aṭṭhamanaya Vippayuttenasampayuttapadaniddesa",
  "dt2.8:1.1_0": "Rūpakkhandhena ye dhammā vippayuttā, te dhammā katihi khandhehi katihāyatanehi katihi dhātūhi sampayuttāti?",
  "dt2.8:1.2_0": "Natthi.",
  "dt2.8:2.1_0": "Vedanākkhandhena ye dhammā …",
  "dt2.8:2.2_0": "saññākkhandhena ye dhammā …",
  "dt2.8:2.3_0": "saṅkhārakkhandhena ye dhammā …",
  "dt2.8:2.4_0": "viññāṇakkhandhena ye dhammā …pe…",
  "dt2.8:2.5_0": "saraṇehi dhammehi ye dhammā …",
  "dt2.8:2.6_0": "araṇehi dhammehi ye dhammā vippayuttā, te dhammā katihi khandhehi katihāyatanehi katihi dhātūhi sampayuttāti?",
  "dt2.8:2.7_0": "Natthi.",
  "dt2.8:3.1_0": "Dhammāyatanaṁ dhammadhātu,",
  "dt2.8:3.2_0": "Atha jīvitaṁ nāmarūpaṁ;",
  "dt2.8:3.3_0": "Saḷāyatanaṁ jātijarāmataṁ,",
  "dt2.8:3.4_0": "Dve ca tike na labbhare.",
  "dt2.8:4.1_0": "Paṭhamantare satta ca,",
  "dt2.8:4.2_0": "Gocchake dasa aparante;",
  "dt2.8:4.3_0": "Cuddasa cha ca matthake,",
  "dt2.8:4.4_0": "Iccete sattacattālīsa dhammā;",
  "dt2.8:4.5_0": "Samucchede na labbhanti,",
  "dt2.8:4.6_0": "Moghapucchakena cāti. [47]",
  "dt2.8:5.1_0": "Vippayuttena sampayuttapadaniddeso aṭṭhamo."
}