{
  "ja126:0.1_0": "Jātaka",
  "ja126:0.2_0": "Ekakanipāta",
  "ja126:0.3_0": "Kusanāḷivagga",
  "ja126:0.4_0": "6. Asilakkhaṇajātaka",
  "ja126:1.1_0": "“Tadevekassa kalyāṇaṁ,",
  "ja126:1.2_0": "tadevekassa pāpakaṁ;",
  "ja126:1.3_0": "Tasmā sabbaṁ na kalyāṇaṁ,",
  "ja126:1.4_0": "sabbaṁ vāpi na pāpakan”ti.",
  "ja126:2.1_0": "Asilakkhaṇajātakaṁ chaṭṭhaṁ."
}