{
  "tha-ap350:0.1_0": "Therāpadāna",
  "tha-ap350:0.2_0": "Ekapadumiyavagga",
  "tha-ap350:0.3_0": "8. Tiṇamuṭṭhidāyakattheraapadāna",
  "tha-ap350:1.1_0": "“Himavantassāvidūre,",
  "tha-ap350:1.2_0": "lambako nāma pabbato;",
  "tha-ap350:1.3_0": "Upatisso nāma sambuddho,",
  "tha-ap350:1.4_0": "abbhokāsamhi caṅkami.",
  "tha-ap350:2.1_0": "Migaluddo tadā āsiṁ,",
  "tha-ap350:2.2_0": "araññe kānane ahaṁ;",
  "tha-ap350:2.3_0": "Disvāna taṁ devadevaṁ,",
  "tha-ap350:2.4_0": "sayambhuṁ aparājitaṁ.",
  "tha-ap350:3.1_0": "Vippasannena cittena,",
  "tha-ap350:3.2_0": "tadā tassa mahesino;",
  "tha-ap350:3.3_0": "Nisīdanatthaṁ buddhassa,",
  "tha-ap350:3.4_0": "tiṇamuṭṭhimadāsahaṁ.",
  "tha-ap350:4.1_0": "Datvāna devadevassa,",
  "tha-ap350:4.2_0": "bhiyyo cittaṁ pasādayiṁ;",
  "tha-ap350:4.3_0": "Sambuddhaṁ abhivādetvā,",
  "tha-ap350:4.4_0": "pakkāmiṁ uttarāmukho.",
  "tha-ap350:5.1_0": "Aciraṁ gatamattaṁ maṁ,",
  "tha-ap350:5.2_0": "migarājā apothayi;",
  "tha-ap350:5.3_0": "Sīhena pothito santo,",
  "tha-ap350:5.4_0": "tattha kālaṅkato ahaṁ.",
  "tha-ap350:6.1_0": "Āsanne me kataṁ kammaṁ,",
  "tha-ap350:6.2_0": "buddhaseṭṭhe anāsave;",
  "tha-ap350:6.3_0": "Sumutto saravegova,",
  "tha-ap350:6.4_0": "devalokaṁ agañchahaṁ.",
  "tha-ap350:7.1_0": "Yūpo tattha subho āsi,",
  "tha-ap350:7.2_0": "puññakammābhinimmito;",
  "tha-ap350:7.3_0": "Sahassakaṇḍo satabheṇḍu,",
  "tha-ap350:7.4_0": "dhajālu haritāmayo.",
  "tha-ap350:8.1_0": "Pabhā niddhāvate tassa,",
  "tha-ap350:8.2_0": "sataraṁsīva uggato;",
  "tha-ap350:8.3_0": "Ākiṇṇo devakaññāhi,",
  "tha-ap350:8.4_0": "āmodiṁ kāmakāmahaṁ.",
  "tha-ap350:9.1_0": "Devalokā cavitvāna,",
  "tha-ap350:9.2_0": "sukkamūlena codito;",
  "tha-ap350:9.3_0": "Āgantvāna manussattaṁ,",
  "tha-ap350:9.4_0": "pattomhi āsavakkhayaṁ.",
  "tha-ap350:10.1_0": "Catunnavutito kappe,",
  "tha-ap350:10.2_0": "nisīdanamadāsahaṁ;",
  "tha-ap350:10.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap350:10.4_0": "tiṇamuṭṭhiyidaṁ phalaṁ.",
  "tha-ap350:11.1_0": "Paṭisambhidā catasso,",
  "tha-ap350:11.2_0": "…pe…",
  "tha-ap350:11.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap350:12.1_0": "Itthaṁ sudaṁ āyasmā tiṇamuṭṭhidāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap350:13.1_0": "Tiṇamuṭṭhidāyakattherassāpadānaṁ aṭṭhamaṁ."
}