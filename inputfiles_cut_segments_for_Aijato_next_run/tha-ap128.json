{
  "tha-ap128:0.1_0": "Therāpadāna",
  "tha-ap128:0.2_0": "Sereyyavagga",
  "tha-ap128:0.3_0": "6. Kusumāsaniyattheraapadāna",
  "tha-ap128:1.1_0": "“Nagare dhaññavatiyā,",
  "tha-ap128:1.2_0": "ahosiṁ brāhmaṇo tadā;",
  "tha-ap128:1.3_0": "Lakkhaṇe itihāse ca,",
  "tha-ap128:1.4_0": "sanighaṇḍusakeṭubhe.",
  "tha-ap128:2.1_0": "Padako veyyākaraṇo,",
  "tha-ap128:2.2_0": "nimittakovido ahaṁ;",
  "tha-ap128:2.3_0": "Mante ca sisse vācesiṁ,",
  "tha-ap128:2.4_0": "tiṇṇaṁ vedāna pāragū.",
  "tha-ap128:3.1_0": "Pañca uppalahatthāni,",
  "tha-ap128:3.2_0": "piṭṭhiyaṁ ṭhapitāni me;",
  "tha-ap128:3.3_0": "Āhutiṁ yiṭṭhukāmohaṁ,",
  "tha-ap128:3.4_0": "pitumātusamāgame.",
  "tha-ap128:4.1_0": "Tadā vipassī bhagavā,",
  "tha-ap128:4.2_0": "bhikkhusaṅghapurakkhato;",
  "tha-ap128:4.3_0": "Obhāsento disā sabbā,",
  "tha-ap128:4.4_0": "āgacchati narāsabho.",
  "tha-ap128:5.1_0": "Āsanaṁ paññapetvāna,",
  "tha-ap128:5.2_0": "nimantetvā mahāmuniṁ;",
  "tha-ap128:5.3_0": "Santharitvāna taṁ pupphaṁ,",
  "tha-ap128:5.4_0": "abhinesiṁ sakaṁ gharaṁ.",
  "tha-ap128:6.1_0": "Yaṁ me atthi sake gehe,",
  "tha-ap128:6.2_0": "āmisaṁ paccupaṭṭhitaṁ;",
  "tha-ap128:6.3_0": "Tāhaṁ buddhassa pādāsiṁ,",
  "tha-ap128:6.4_0": "pasanno sehi pāṇibhi.",
  "tha-ap128:7.1_0": "Bhuttāviṁ kālamaññāya,",
  "tha-ap128:7.2_0": "pupphahatthamadāsahaṁ;",
  "tha-ap128:7.3_0": "Anumoditvāna sabbaññū,",
  "tha-ap128:7.4_0": "pakkāmi uttarāmukho.",
  "tha-ap128:8.1_0": "Ekanavutito kappe,",
  "tha-ap128:8.2_0": "yaṁ pupphamadadiṁ tadā;",
  "tha-ap128:8.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap128:8.4_0": "pupphadānassidaṁ phalaṁ.",
  "tha-ap128:9.1_0": "Anantaraṁ ito kappe,",
  "tha-ap128:9.2_0": "rājāhuṁ varadassano;",
  "tha-ap128:9.3_0": "Sattaratanasampanno,",
  "tha-ap128:9.4_0": "cakkavattī mahabbalo.",
  "tha-ap128:10.1_0": "Paṭisambhidā catasso,",
  "tha-ap128:10.2_0": "…pe…",
  "tha-ap128:10.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap128:11.1_0": "Itthaṁ sudaṁ āyasmā kusumāsaniyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap128:12.1_0": "Kusumāsaniyattherassāpadānaṁ chaṭṭhaṁ."
}