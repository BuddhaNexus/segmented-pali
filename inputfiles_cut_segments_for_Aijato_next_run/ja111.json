{
  "ja111:0.1_0": "Jātaka",
  "ja111:0.2_0": "Ekakanipāta",
  "ja111:0.3_0": "Haṁcivagga",
  "ja111:0.4_0": "1. Gadrabhapañhajātaka",
  "ja111:1.1_0": "“Haṁci tuvaṁ evamaññasi seyyo,",
  "ja111:1.2_0": "Puttena pitāti rājaseṭṭha;",
  "ja111:1.3_0": "Handassatarassa te ayaṁ,",
  "ja111:1.4_0": "Assatarassa hi gadrabho pitā”ti.",
  "ja111:2.1_0": "Gadrabhapañhajātakaṁ paṭhamaṁ."
}