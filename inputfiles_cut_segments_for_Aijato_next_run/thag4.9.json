{
  "thag4.9:0.1_0": "Theragāthā",
  "thag4.9:0.2_0": "Catukkanipāta",
  "thag4.9:0.3_0": "Paṭhamavagga",
  "thag4.9:0.4_0": "4.9. Candanattheragāthā",
  "thag4.9:1.1_0": "“Jātarūpena sañchannā,",
  "thag4.9:1.2_0": "dāsīgaṇapurakkhatā;",
  "thag4.9:1.3_0": "Aṅkena puttamādāya,",
  "thag4.9:1.4_0": "bhariyā maṁ upāgami.",
  "thag4.9:2.1_0": "Tañca disvāna āyantiṁ,",
  "thag4.9:2.2_0": "sakaputtassa mātaraṁ;",
  "thag4.9:2.3_0": "Alaṅkataṁ suvasanaṁ,",
  "thag4.9:2.4_0": "maccupāsaṁva oḍḍitaṁ.",
  "thag4.9:3.1_0": "Tato me manasīkāro,",
  "thag4.9:3.2_0": "yoniso udapajjatha;",
  "thag4.9:3.3_0": "Ādīnavo pāturahu,",
  "thag4.9:3.4_0": "nibbidā samatiṭṭhatha.",
  "thag4.9:4.1_0": "Tato cittaṁ vimucci me,",
  "thag4.9:4.2_0": "passa dhammasudhammataṁ;",
  "thag4.9:4.3_0": "Tisso vijjā anuppattā,",
  "thag4.9:4.4_0": "kataṁ buddhassa sāsanan”ti.",
  "thag4.9:4.5_0": "… Candano thero …."
}