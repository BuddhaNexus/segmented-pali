{
  "ja456:0.1_0": "Jātaka",
  "ja456:0.2_0": "Ekādasakanipāta",
  "ja456:0.3_0": "Mātuposakavagga",
  "ja456:0.4_0": "2. Juṇhajātaka",
  "ja456:1.1_0": "“Suṇohi mayhaṁ vacanaṁ janinda,",
  "ja456:1.2_0": "Atthena juṇhamhi idhānupatto;",
  "ja456:1.3_0": "Na brāhmaṇe addhike tiṭṭhamāne,",
  "ja456:1.4_0": "Gantabbamāhu dvipadindaseṭṭha”.",
  "ja456:2.1_0": "“Suṇomi tiṭṭhāmi vadehi brahme,",
  "ja456:2.2_0": "Yenāsi atthena idhānupatto;",
  "ja456:2.3_0": "Kaṁ vā tvamatthaṁ mayi patthayāno,",
  "ja456:2.4_0": "Idhāgamā brahme tadiṅgha brūhi”.",
  "ja456:3.1_0": "“Dadāhi me gāmavarāni pañca,",
  "ja456:3.2_0": "Dāsīsataṁ satta gavaṁsatāni;",
  "ja456:3.3_0": "Parosahassañca suvaṇṇanikkhe,",
  "ja456:3.4_0": "Bhariyā ca me sādisī dve dadāhi.",
  "ja456:4.1_0": "Tapo nu te brāhmaṇa bhiṁsarūpo,",
  "ja456:4.2_0": "Mantā nu te brāhmaṇa cittarūpā;",
  "ja456:4.3_0": "Yakkhā nu te assavā santi keci,",
  "ja456:4.4_0": "Atthaṁ vā me abhijānāsi kattaṁ.",
  "ja456:5.1_0": "Na me tapo atthi na cāpi mantā,",
  "ja456:5.2_0": "Yakkhāpi me assavā natthi keci;",
  "ja456:5.3_0": "Atthampi te nābhijānāmi kattaṁ,",
  "ja456:5.4_0": "Pubbe ca kho saṅgatimattamāsi.",
  "ja456:6.1_0": "Paṭhamaṁ idaṁ dassanaṁ jānato me,",
  "ja456:6.2_0": "Na tābhijānāmi ito puratthā;",
  "ja456:6.3_0": "Akkhāhi me pucchito etamatthaṁ,",
  "ja456:6.4_0": "Kadā kuhiṁ vā ahu saṅgamo no.",
  "ja456:7.1_0": "Gandhārarājassa puramhi ramme,",
  "ja456:7.2_0": "Avasimhase takkasīlāyaṁ deva;",
  "ja456:7.3_0": "Tatthandhakāramhi timīsikāyaṁ,",
  "ja456:7.4_0": "Aṁsena aṁsaṁ samaghaṭṭayimha.",
  "ja456:8.1_0": "Te tattha ṭhatvāna ubho janinda,",
  "ja456:8.2_0": "Sārāṇiyaṁ vītisārayimha tattha;",
  "ja456:8.3_0": "Sāyeva no saṅgatimattamāsi,",
  "ja456:8.4_0": "Tato na pacchā na pure ahosi.",
  "ja456:9.1_0": "Yadā kadāci manujesu brahme,",
  "ja456:9.2_0": "Samāgamo sappurisena hoti;",
  "ja456:9.3_0": "Na paṇḍitā saṅgatisanthavāni,",
  "ja456:9.4_0": "Pubbe kataṁ vāpi vināsayanti.",
  "ja456:10.1_0": "Bālāva kho saṅgatisanthavāni,",
  "ja456:10.2_0": "Pubbe kataṁ vāpi vināsayanti;",
  "ja456:10.3_0": "Bahumpi bālesu kataṁ vinassati,",
  "ja456:10.4_0": "Tathā hi bālā akataññurūpā.",
  "ja456:11.1_0": "Dhīrā ca kho saṅgatisanthavāni,",
  "ja456:11.2_0": "Pubbe kataṁ vāpi na nāsayanti;",
  "ja456:11.3_0": "Appampi dhīresu kataṁ na nassati,",
  "ja456:11.4_0": "Tathā hi dhīrā sukataññurūpā.",
  "ja456:12.1_0": "Dadāmi te gāmavarāni pañca,",
  "ja456:12.2_0": "Dāsīsataṁ satta gavaṁsatāni;",
  "ja456:12.3_0": "Parosahassañca suvaṇṇanikkhe,",
  "ja456:12.4_0": "Bhariyā ca te sādisī dve dadāmi.",
  "ja456:13.1_0": "Evaṁ sataṁ hoti samecca rāja,",
  "ja456:13.2_0": "Nakkhattarājāriva tārakānaṁ;",
  "ja456:13.3_0": "Āpūratī kāsipatī tathāhaṁ,",
  "ja456:13.4_0": "Tayāpi me saṅgamo ajja laddho”ti.",
  "ja456:14.1_0": "Juṇhajātakaṁ dutiyaṁ."
}