{
  "ja197:0.1_0": "Jātaka",
  "ja197:0.2_0": "Dukanipāta",
  "ja197:0.3_0": "Ruhakavagga",
  "ja197:0.4_0": "7. Mittāmittajātaka",
  "ja197:1.1_0": "“Na naṁ umhayate disvā,",
  "ja197:1.2_0": "na ca naṁ paṭinandati;",
  "ja197:1.3_0": "Cakkhūni cassa na dadāti,",
  "ja197:1.4_0": "paṭilomañca vattati.",
  "ja197:2.1_0": "Ete bhavanti ākārā,",
  "ja197:2.2_0": "amittasmiṁ patiṭṭhitā;",
  "ja197:2.3_0": "Yehi amittaṁ jāneyya,",
  "ja197:2.4_0": "disvā sutvā ca paṇḍito”ti.",
  "ja197:3.1_0": "Mittāmittajātakaṁ sattamaṁ."
}