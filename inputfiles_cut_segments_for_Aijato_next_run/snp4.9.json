{
  "snp4.9:0.1_0": "Sutta Nipāta 4.9",
  "snp4.9:0.2_0": "Māgaṇḍiyasutta",
  "snp4.9:1.1_0": "“Disvāna taṇhaṁ aratiṁ ragañca,",
  "snp4.9:1.2_0": "Nāhosi chando api methunasmiṁ;",
  "snp4.9:1.3_0": "Kimevidaṁ muttakarīsapuṇṇaṁ,",
  "snp4.9:1.4_0": "Pādāpi naṁ samphusituṁ na icche”.",
  "snp4.9:2.1_0": "“Etādisañce ratanaṁ na icchasi,",
  "snp4.9:2.2_0": "Nāriṁ narindehi bahūhi patthitaṁ;",
  "snp4.9:2.3_0": "Diṭṭhigataṁ sīlavataṁ nu jīvitaṁ,",
  "snp4.9:2.4_0": "Bhavūpapattiñca vadesi kīdisaṁ”.",
  "snp4.9:3.1_0": "“Idaṁ vadāmīti na tassa hoti,",
  "snp4.9:3.2_0": "(māgaṇḍiyāti bhagavā)",
  "snp4.9:3.3_0": "Dhammesu niccheyya samuggahītaṁ;",
  "snp4.9:3.4_0": "Passañca diṭṭhīsu anuggahāya,",
  "snp4.9:3.5_0": "Ajjhattasantiṁ pacinaṁ adassaṁ”.",
  "snp4.9:4.1_0": "“Vinicchayā yāni pakappitāni,",
  "snp4.9:4.2_0": "(iti māgaṇḍiyo)",
  "snp4.9:4.3_0": "Te ve munī brūsi anuggahāya;",
  "snp4.9:4.4_0": "Ajjhattasantīti yametamatthaṁ,",
  "snp4.9:4.5_0": "Kathaṁ nu dhīrehi paveditaṁ taṁ”.",
  "snp4.9:5.1_0": "“Na diṭṭhiyā na sutiyā na ñāṇena,",
  "snp4.9:5.2_0": "(māgaṇḍiyāti bhagavā)",
  "snp4.9:5.3_0": "Sīlabbatenāpi na suddhimāha;",
  "snp4.9:5.4_0": "Adiṭṭhiyā assutiyā añāṇā,",
  "snp4.9:5.5_0": "Asīlatā abbatā nopi tena;",
  "snp4.9:5.6_0": "Ete ca nissajja anuggahāya,",
  "snp4.9:5.7_0": "Santo anissāya bhavaṁ na jappe”.",
  "snp4.9:6.1_0": "“No ce kira diṭṭhiyā na sutiyā na ñāṇena,",
  "snp4.9:6.2_0": "(iti māgaṇḍiyo)",
  "snp4.9:6.3_0": "Sīlabbatenāpi na suddhimāha;",
  "snp4.9:6.4_0": "Adiṭṭhiyā assutiyā añāṇā,",
  "snp4.9:6.5_0": "Asīlatā abbatā nopi tena;",
  "snp4.9:6.6_0": "Maññāmahaṁ momuhameva dhammaṁ,",
  "snp4.9:6.7_0": "Diṭṭhiyā eke paccenti suddhiṁ”.",
  "snp4.9:7.1_0": "“Diṭṭhañca nissāya anupucchamāno,",
  "snp4.9:7.2_0": "(māgaṇḍiyāti bhagavā)",
  "snp4.9:7.3_0": "Samuggahītesu pamohamāgā;",
  "snp4.9:7.4_0": "Ito ca nāddakkhi aṇumpi saññaṁ,",
  "snp4.9:7.5_0": "Tasmā tuvaṁ momuhato dahāsi.",
  "snp4.9:8.1_0": "Samo visesī uda vā nihīno,",
  "snp4.9:8.2_0": "Yo maññati so vivadetha tena;",
  "snp4.9:8.3_0": "Tīsu vidhāsu avikampamāno,",
  "snp4.9:8.4_0": "Samo visesīti na tassa hoti.",
  "snp4.9:9.1_0": "Saccanti so brāhmaṇo kiṁ vadeyya,",
  "snp4.9:9.2_0": "Musāti vā so vivadetha kena;",
  "snp4.9:9.3_0": "Yasmiṁ samaṁ visamaṁ vāpi natthi,",
  "snp4.9:9.4_0": "Sa kena vādaṁ paṭisaṁyujeyya.",
  "snp4.9:10.1_0": "Okaṁ pahāya aniketasārī,",
  "snp4.9:10.2_0": "Gāme akubbaṁ muni santhavāni;",
  "snp4.9:10.3_0": "Kāmehi ritto apurakkharāno,",
  "snp4.9:10.4_0": "Kathaṁ na viggayha janena kayirā.",
  "snp4.9:11.1_0": "Yehi vivitto vicareyya loke,",
  "snp4.9:11.2_0": "Na tāni uggayha vadeyya nāgo;",
  "snp4.9:11.3_0": "Jalambujaṁ kaṇḍakavārijaṁ yathā,",
  "snp4.9:11.4_0": "Jalena paṅkena canūpalittaṁ;",
  "snp4.9:11.5_0": "Evaṁ munī santivādo agiddho,",
  "snp4.9:11.6_0": "Kāme ca loke ca anūpalitto.",
  "snp4.9:12.1_0": "Na vedagū diṭṭhiyāyako na mutiyā,",
  "snp4.9:12.2_0": "Sa mānameti na hi tammayo so;",
  "snp4.9:12.3_0": "Na kammunā nopi sutena neyyo,",
  "snp4.9:12.4_0": "Anūpanīto sa nivesanesu.",
  "snp4.9:13.1_0": "Saññāvirattassa na santi ganthā,",
  "snp4.9:13.2_0": "Paññāvimuttassa na santi mohā;",
  "snp4.9:13.3_0": "Saññañca diṭṭhiñca ye aggahesuṁ,",
  "snp4.9:13.4_0": "Te ghaṭṭayantā vicaranti loke”ti.",
  "snp4.9:13.5_0": "Māgaṇḍiyasuttaṁ navamaṁ."
}