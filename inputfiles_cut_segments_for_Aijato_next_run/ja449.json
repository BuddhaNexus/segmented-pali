{
  "ja449:0.1_0": "Jātaka",
  "ja449:0.2_0": "Dasakanipāta",
  "ja449:0.3_0": "Catudvāravagga",
  "ja449:0.4_0": "11. Maṭṭhakuṇḍalījātaka",
  "ja449:1.1_0": "“Alaṅkato maṭṭhakuṇḍalī,",
  "ja449:1.2_0": "Māladhārī haricandanussado;",
  "ja449:1.3_0": "Bāhā paggayha kandasi,",
  "ja449:1.4_0": "Vanamajjhe kiṁ dukkhito tuvaṁ”.",
  "ja449:2.1_0": "“Sovaṇṇamayo pabhassaro,",
  "ja449:2.2_0": "Uppanno rathapañjaro mama;",
  "ja449:2.3_0": "Tassa cakkayugaṁ na vindāmi,",
  "ja449:2.4_0": "Tena dukkhena jahāmi jīvitaṁ”.",
  "ja449:3.1_0": "“Sovaṇṇamayaṁ maṇīmayaṁ,",
  "ja449:3.2_0": "Lohamayaṁ atha rūpiyāmayaṁ;",
  "ja449:3.3_0": "Pāvada rathaṁ karissāmi te,",
  "ja449:3.4_0": "Cakkayugaṁ paṭipādayāmi taṁ”.",
  "ja449:4.1_0": "“So māṇavo tassa pāvadi,",
  "ja449:4.2_0": "Candasūriyā ubhayettha bhātaro;",
  "ja449:4.3_0": "Sovaṇṇamayo ratho mama,",
  "ja449:4.4_0": "Tena cakkayugena sobhati”.",
  "ja449:5.1_0": "“Bālo kho tvaṁsi māṇava,",
  "ja449:5.2_0": "Yo tvaṁ patthayase apatthiyaṁ;",
  "ja449:5.3_0": "Maññāmi tuvaṁ marissasi,",
  "ja449:5.4_0": "Na hi tvaṁ lacchasi candasūriye”.",
  "ja449:6.1_0": "“Gamanāgamanampi dissati,",
  "ja449:6.2_0": "Vaṇṇadhātu ubhayettha vīthiyo;",
  "ja449:6.3_0": "Peto pana neva dissati,",
  "ja449:6.4_0": "Ko nu kho kandataṁ bālyataro”.",
  "ja449:7.1_0": "“Saccaṁ kho vadesi māṇava,",
  "ja449:7.2_0": "Ahameva kandataṁ bālyataro;",
  "ja449:7.3_0": "Candaṁ viya dārako rudaṁ,",
  "ja449:7.4_0": "Petaṁ kālakatābhipatthaye.",
  "ja449:8.1_0": "Ādittaṁ vata maṁ santaṁ,",
  "ja449:8.2_0": "ghatasittaṁva pāvakaṁ;",
  "ja449:8.3_0": "Vārinā viya osiñcaṁ,",
  "ja449:8.4_0": "sabbaṁ nibbāpaye daraṁ.",
  "ja449:9.1_0": "Abbahī vata me sallaṁ,",
  "ja449:9.2_0": "yamāsi hadayassitaṁ;",
  "ja449:9.3_0": "Yo me sokaparetassa,",
  "ja449:9.4_0": "puttasokaṁ apānudi.",
  "ja449:10.1_0": "Sohaṁ abbūḷhasallosmi,",
  "ja449:10.2_0": "vītasoko anāvilo;",
  "ja449:10.3_0": "Na socāmi na rodāmi,",
  "ja449:10.4_0": "tava sutvāna māṇavā”ti.",
  "ja449:11.1_0": "Maṭṭhakuṇḍalījātakaṁ ekādasamaṁ."
}