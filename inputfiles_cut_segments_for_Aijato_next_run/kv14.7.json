{
  "kv14.7:0.1_0": "Kathāvatthu",
  "kv14.7:0.2_0": "Mahāpaṇṇāsaka",
  "kv14.7:0.3_0": "Cuddasamavagga",
  "kv14.7:0.4_0": "Pariyāpannakathā",
  "kv14.7:1.1_0": "Rūparāgo rūpadhātuṁ anuseti, rūpadhātupariyāpannoti?",
  "kv14.7:1.2_0": "Āmantā.",
  "kv14.7:1.3_0": "Samāpattesiyo upapattesiyo diṭṭhadhammasukhavihāro, samāpattesiyena cittena upapattesiyena cittena diṭṭhadhammasukhavihārena cittena sahagato sahajāto saṁsaṭṭho sampayutto ekuppādo ekanirodho ekavatthuko ekārammaṇoti?",
  "kv14.7:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv14.7:1.5_0": "nanu na samāpattesiyo na upapattesiyo na diṭṭhadhammasukhavihāro, na samāpattesiyena cittena na upapattesiyena cittena na diṭṭhadhammasukhavihārena cittena sahagato sahajāto saṁsaṭṭho sampayutto ekuppādo ekanirodho ekavatthuko ekārammaṇoti?",
  "kv14.7:1.6_0": "Āmantā.",
  "kv14.7:1.7_0": "Hañci na samāpattesiyo na upapattesiyo na diṭṭhadhammasukhavihāro …pe…",
  "kv14.7:1.7_1": "ekārammaṇo, no ca vata re vattabbe—",
  "kv14.7:1.8_0": "“rūparāgo rūpadhātuṁ anuseti, rūpadhātupariyāpanno”ti …pe….",
  "kv14.7:2.1_0": "Rūparāgo rūpadhātuṁ anuseti, rūpadhātupariyāpannoti?",
  "kv14.7:2.2_0": "Āmantā.",
  "kv14.7:2.3_0": "Saddarāgo saddadhātuṁ anuseti, saddadhātupariyāpannoti?",
  "kv14.7:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv14.7:2.5_0": "rūparāgo rūpadhātuṁ anuseti, rūpadhātupariyāpannoti?",
  "kv14.7:2.6_0": "Āmantā.",
  "kv14.7:2.7_0": "Gandharāgo …pe…",
  "kv14.7:2.8_0": "rasarāgo …pe…",
  "kv14.7:2.9_0": "phoṭṭhabbarāgo phoṭṭhabbadhātuṁ anuseti, phoṭṭhabbadhātupariyāpannoti?",
  "kv14.7:2.10_0": "Na hevaṁ vattabbe …pe….",
  "kv14.7:3.1_0": "Saddarāgo saddadhātuṁ anuseti, na vattabbaṁ—",
  "kv14.7:3.2_0": "“saddadhātupariyāpanno”ti?",
  "kv14.7:3.3_0": "Āmantā.",
  "kv14.7:3.4_0": "Rūparāgo rūpadhātuṁ anuseti, na vattabbaṁ—",
  "kv14.7:3.5_0": "“rūpadhātupariyāpanno”ti?",
  "kv14.7:3.6_0": "Na hevaṁ vattabbe …pe…",
  "kv14.7:3.7_0": "gandharāgo …pe…",
  "kv14.7:3.8_0": "rasarāgo …pe…",
  "kv14.7:3.9_0": "phoṭṭhabbarāgo phoṭṭhabbadhātuṁ anuseti, na vattabbaṁ—",
  "kv14.7:3.10_0": "“phoṭṭhabbadhātupariyāpanno”ti?",
  "kv14.7:3.11_0": "Āmantā.",
  "kv14.7:3.12_0": "Rūparāgo rūpadhātuṁ anuseti, na vattabbaṁ—",
  "kv14.7:3.13_0": "“rūpadhātupariyāpanno”ti?",
  "kv14.7:3.14_0": "Na hevaṁ vattabbe …pe….",
  "kv14.7:4.1_0": "Arūparāgo arūpadhātuṁ anuseti, arūpadhātupariyāpannoti?",
  "kv14.7:4.2_0": "Āmantā.",
  "kv14.7:4.3_0": "Samāpattesiyo upapattesiyo diṭṭhadhammasukhavihāro, samāpattesiyena cittena upapattesiyena cittena diṭṭhadhammasukhavihārena cittena sahagato sahajāto saṁsaṭṭho sampayutto ekuppādo ekanirodho ekavatthuko ekārammaṇoti?",
  "kv14.7:4.4_0": "Na hevaṁ vattabbe …pe….",
  "kv14.7:4.5_0": "Nanu na samāpattesiyo na upapattesiyo na diṭṭhadhammasukhavihāro, na samāpattesiyena cittena …pe…",
  "kv14.7:4.5_1": "ekārammaṇoti?",
  "kv14.7:4.6_0": "Āmantā.",
  "kv14.7:4.7_0": "Hañci na samāpattesiyo na upapattesiyo …pe…",
  "kv14.7:4.8_0": "ekārammaṇo, no ca vata re vattabbe—",
  "kv14.7:4.9_0": "“arūparāgo arūpadhātuṁ anuseti, arūpadhātupariyāpanno”ti.",
  "kv14.7:5.1_0": "Arūparāgo arūpadhātuṁ anuseti, arūpadhātupariyāpannoti?",
  "kv14.7:5.2_0": "Āmantā.",
  "kv14.7:5.3_0": "Saddarāgo saddadhātuṁ anuseti, saddadhātupariyāpannoti?",
  "kv14.7:5.4_0": "Na hevaṁ vattabbe …pe…",
  "kv14.7:5.5_0": "arūparāgo arūpadhātuṁ anuseti, arūpadhātupariyāpannoti?",
  "kv14.7:5.6_0": "Āmantā.",
  "kv14.7:5.7_0": "Gandharāgo …pe…",
  "kv14.7:5.8_0": "rasarāgo …pe…",
  "kv14.7:5.9_0": "phoṭṭhabbarāgo phoṭṭhabbadhātuṁ anuseti, phoṭṭhabbadhātupariyāpannoti?",
  "kv14.7:5.10_0": "Na hevaṁ vattabbe …pe….",
  "kv14.7:6.1_0": "Saddarāgo saddadhātuṁ anuseti, na vattabbaṁ—",
  "kv14.7:6.2_0": "“saddadhātupariyāpanno”ti?",
  "kv14.7:6.3_0": "Āmantā.",
  "kv14.7:6.4_0": "Arūparāgo arūpadhātuṁ anuseti, na vattabbaṁ—",
  "kv14.7:6.5_0": "“arūpadhātupariyāpanno”ti, na hevaṁ vattabbe …pe…",
  "kv14.7:6.6_0": "gandharāgo …pe…",
  "kv14.7:6.7_0": "rasarāgo …pe…",
  "kv14.7:6.8_0": "phoṭṭhabbarāgo phoṭṭhabbadhātuṁ anuseti, na vattabbaṁ—",
  "kv14.7:6.9_0": "“phoṭṭhabbadhātupariyāpanno”ti?",
  "kv14.7:6.10_0": "Āmantā.",
  "kv14.7:6.11_0": "Arūparāgo arūpadhātuṁ anuseti, na vattabbaṁ—",
  "kv14.7:6.12_0": "“arūpadhātupariyāpanno”ti?",
  "kv14.7:6.13_0": "Na hevaṁ vattabbe …pe….",
  "kv14.7:7.1_0": "Na vattabbaṁ—",
  "kv14.7:7.2_0": "“rūparāgo rūpadhātuṁ anuseti rūpadhātupariyāpanno, arūparāgo arūpadhātuṁ anuseti arūpadhātupariyāpanno”ti?",
  "kv14.7:7.3_0": "Āmantā.",
  "kv14.7:7.4_0": "Nanu kāmarāgo kāmadhātuṁ anuseti, kāmadhātupariyāpannoti?",
  "kv14.7:7.5_0": "Āmantā.",
  "kv14.7:7.6_0": "Hañci kāmarāgo kāmadhātuṁ anuseti kāmadhātupariyāpanno, tena vata re vattabbe—",
  "kv14.7:7.7_0": "“rūparāgo rūpadhātuṁ anuseti rūpadhātupariyāpanno, arūparāgo arūpadhātuṁ anuseti arūpadhātupariyāpanno”ti.",
  "kv14.7:7.8_0": "Pariyāpannakathā niṭṭhitā."
}