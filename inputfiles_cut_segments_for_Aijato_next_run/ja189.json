{
  "ja189:0.1_0": "Jātaka",
  "ja189:0.2_0": "Dukanipāta",
  "ja189:0.3_0": "Asadisavagga",
  "ja189:0.4_0": "9. Sīhacammajātaka",
  "ja189:1.1_0": "“Netaṁ sīhassa naditaṁ,",
  "ja189:1.2_0": "na byagghassa na dīpino;",
  "ja189:1.3_0": "Pāruto sīhacammena,",
  "ja189:1.4_0": "jammo nadati gadrabho”.",
  "ja189:2.1_0": "“Cirampi kho taṁ khādeyya,",
  "ja189:2.2_0": "gadrabho haritaṁ yavaṁ;",
  "ja189:2.3_0": "Pāruto sīhacammena,",
  "ja189:2.4_0": "ravamānova dūsayī”ti.",
  "ja189:3.1_0": "Sīhacammajātakaṁ navamaṁ."
}