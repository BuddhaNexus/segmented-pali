{
  "tha-ap56:0.1_0": "Therāpadāna",
  "tha-ap56:0.2_0": "Bījanivagga",
  "tha-ap56:0.3_0": "4 Gandhodakiyattheraapadāna",
  "tha-ap56:1.1_0": "“Padumuttarabuddhassa,",
  "tha-ap56:1.2_0": "mahābodhimaho ahu;",
  "tha-ap56:1.3_0": "Vicittaṁ ghaṭamādāya,",
  "tha-ap56:1.4_0": "gandhodakamadāsahaṁ.",
  "tha-ap56:2.1_0": "Nhānakāle ca bodhiyā,",
  "tha-ap56:2.2_0": "mahāmegho pavassatha;",
  "tha-ap56:2.3_0": "Ninnādo ca mahā āsi,",
  "tha-ap56:2.4_0": "asaniyā phalantiyā.",
  "tha-ap56:3.1_0": "Tenevāsanivegena,",
  "tha-ap56:3.2_0": "tattha kālaṅkato ahaṁ;",
  "tha-ap56:3.3_0": "Devaloke ṭhito santo,",
  "tha-ap56:3.4_0": "imā gāthā abhāsahaṁ.",
  "tha-ap56:4.1_0": "‘Aho buddhā aho dhammā,",
  "tha-ap56:4.2_0": "aho no satthusampadā;",
  "tha-ap56:4.3_0": "Kaḷevaraṁ me patitaṁ,",
  "tha-ap56:4.4_0": "devaloke ramāmahaṁ.",
  "tha-ap56:5.1_0": "Ubbiddhaṁ bhavanaṁ mayhaṁ,",
  "tha-ap56:5.2_0": "Satabhūmaṁ samuggataṁ;",
  "tha-ap56:5.3_0": "Kaññāsatasahassāni,",
  "tha-ap56:5.4_0": "Parivārenti maṁ sadā.",
  "tha-ap56:6.1_0": "Ābādhā me na vijjanti,",
  "tha-ap56:6.2_0": "soko mayhaṁ na vijjati;",
  "tha-ap56:6.3_0": "Pariḷāhaṁ na passāmi,",
  "tha-ap56:6.4_0": "puññakammassidaṁ phalaṁ.",
  "tha-ap56:7.1_0": "Aṭṭhavīse kappasate,",
  "tha-ap56:7.2_0": "rājā saṁvasito ahuṁ;",
  "tha-ap56:7.3_0": "Sattaratanasampanno,",
  "tha-ap56:7.4_0": "cakkavattī mahabbalo’.",
  "tha-ap56:8.1_0": "Paṭisambhidā catasso,",
  "tha-ap56:8.2_0": "…pe…",
  "tha-ap56:8.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap56:9.1_0": "Itthaṁ sudaṁ āyasmā gandhodakiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap56:10.1_0": "Gandhodakiyattherassāpadānaṁ catutthaṁ."
}