{
  "ja41:0.1_0": "Jātaka",
  "ja41:0.2_0": "Ekakanipāta",
  "ja41:0.3_0": "Atthakāmavagga",
  "ja41:0.4_0": "1. Losakajātaka",
  "ja41:1.1_0": "“Yo atthakāmassa hitānukampino,",
  "ja41:1.2_0": "Ovajjamāno na karoti sāsanaṁ;",
  "ja41:1.3_0": "Ajiyā pādamolamba,",
  "ja41:1.4_0": "Mittako viya socatī”ti.",
  "ja41:2.1_0": "Losakajātakaṁ paṭhamaṁ."
}