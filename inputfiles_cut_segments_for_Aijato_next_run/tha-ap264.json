{
  "tha-ap264:0.1_0": "Therāpadāna",
  "tha-ap264:0.2_0": "Padumukkhipavagga",
  "tha-ap264:0.3_0": "2. Telamakkhiyattheraapadāna",
  "tha-ap264:1.1_0": "“Siddhatthamhi bhagavati,",
  "tha-ap264:1.2_0": "nibbutamhi narāsabhe;",
  "tha-ap264:1.3_0": "Bodhiyā vedikāyāhaṁ,",
  "tha-ap264:1.4_0": "telaṁ makkhesi tāvade.",
  "tha-ap264:2.1_0": "Catunnavutito kappe,",
  "tha-ap264:2.2_0": "yaṁ telaṁ makkhayiṁ tadā;",
  "tha-ap264:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap264:2.4_0": "makkhanāya idaṁ phalaṁ.",
  "tha-ap264:3.1_0": "Catuvīse ito kappe,",
  "tha-ap264:3.2_0": "succhavi nāma khattiyo;",
  "tha-ap264:3.3_0": "Sattaratanasampanno,",
  "tha-ap264:3.4_0": "cakkavattī mahabbalo.",
  "tha-ap264:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap264:4.2_0": "…pe…",
  "tha-ap264:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap264:5.1_0": "Itthaṁ sudaṁ āyasmā telamakkhiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap264:6.1_0": "Telamakkhiyattherassāpadānaṁ dutiyaṁ."
}