{
  "bv1:0.1_0": "Buddhavaṁsa",
  "bv1:0.2_0": "1. Ratanacaṅkamanakaṇḍa",
  "bv1:1.1_0": "Namo tassa Bhagavato Arahato Sammāsambuddhassa.",
  "bv1:2.1_0": "Brahmā ca lokādhipatī sahampatī,",
  "bv1:2.2_0": "Katañjalī anadhivaraṁ ayācatha;",
  "bv1:2.3_0": "“Santīdha sattāpparajakkhajātikā,",
  "bv1:2.4_0": "Desehi dhammaṁ anukampimaṁ pajaṁ”.",
  "bv1:3.1_0": "Sampannavijjācaraṇassa tādino,",
  "bv1:3.2_0": "Jutindharassantimadehadhārino;",
  "bv1:3.3_0": "Tathāgatassappaṭipuggalassa,",
  "bv1:3.4_0": "Uppajji kāruññatā sabbasatte.",
  "bv1:4.1_0": "“Na hete jānanti sadevamānusā,",
  "bv1:4.2_0": "Buddho ayaṁ kīdisako naruttamo;",
  "bv1:4.3_0": "Iddhibalaṁ paññābalañca kīdisaṁ,",
  "bv1:4.4_0": "Buddhabalaṁ lokahitassa kīdisaṁ.",
  "bv1:5.1_0": "Na hete jānanti sadevamānusā,",
  "bv1:5.2_0": "Buddho ayaṁ edisako naruttamo;",
  "bv1:5.3_0": "Iddhibalaṁ paññābalañca edisaṁ,",
  "bv1:5.4_0": "Buddhabalaṁ lokahitassa edisaṁ.",
  "bv1:6.1_0": "Handāhaṁ dassayissāmi,",
  "bv1:6.2_0": "buddhabalamanuttaraṁ;",
  "bv1:6.3_0": "Caṅkamaṁ māpayissāmi,",
  "bv1:6.4_0": "nabhe ratanamaṇḍitaṁ”.",
  "bv1:7.1_0": "Bhummā mahārājikā tāvatiṁsā,",
  "bv1:7.2_0": "Yāmā ca devā tusitā ca nimmitā;",
  "bv1:7.3_0": "Paranimmitā yepi ca brahmakāyikā,",
  "bv1:7.4_0": "Ānanditā vipulamakaṁsu ghosaṁ.",
  "bv1:8.1_0": "Obhāsitā ca pathavī sadevakā,",
  "bv1:8.2_0": "Puthū ca lokantarikā asaṁvutā;",
  "bv1:8.3_0": "Tamo ca tibbo vihato tadā ahu,",
  "bv1:8.4_0": "Disvāna accherakaṁ pāṭihīraṁ.",
  "bv1:9.1_0": "Sadevagandhabbamanussarakkhase,",
  "bv1:9.2_0": "Ābhā uḷārā vipulā ajāyatha;",
  "bv1:9.3_0": "Imasmiṁ loke parasmiñcobhayasmiṁ,",
  "bv1:9.4_0": "Adho ca uddhaṁ tiriyañca vitthataṁ.",
  "bv1:10.1_0": "Sattuttamo anadhivaro vināyako,",
  "bv1:10.2_0": "Satthā ahū devamanussapūjito;",
  "bv1:10.3_0": "Mahānubhāvo satapuññalakkhaṇo,",
  "bv1:10.4_0": "Dassesi accherakaṁ pāṭihīraṁ.",
  "bv1:11.1_0": "So yācito devavarena cakkhumā,",
  "bv1:11.2_0": "Atthaṁ samekkhitvā tadā naruttamo;",
  "bv1:11.3_0": "Caṅkamaṁ māpayi lokanāyako,",
  "bv1:11.4_0": "Suniṭṭhitaṁ sabbaratananimmitaṁ.",
  "bv1:12.1_0": "Iddhī ca ādesanānusāsanī,",
  "bv1:12.2_0": "Tipāṭihīre bhagavā vasī ahu;",
  "bv1:12.3_0": "Caṅkamaṁ māpayi lokanāyako,",
  "bv1:12.4_0": "Suniṭṭhitaṁ sabbaratananimmitaṁ.",
  "bv1:13.1_0": "Dasasahassīlokadhātuyā,",
  "bv1:13.2_0": "Sinerupabbatuttame;",
  "bv1:13.3_0": "Thambheva dassesi paṭipāṭiyā,",
  "bv1:13.4_0": "Caṅkame ratanāmaye.",
  "bv1:14.1_0": "Dasasahassī atikkamma,",
  "bv1:14.2_0": "caṅkamaṁ māpayī jino;",
  "bv1:14.3_0": "Sabbasoṇṇamayā passe,",
  "bv1:14.4_0": "caṅkame ratanāmaye.",
  "bv1:15.1_0": "Tulāsaṅghāṭānuvaggā,",
  "bv1:15.2_0": "sovaṇṇaphalakatthatā;",
  "bv1:15.3_0": "Vedikā sabbasovaṇṇā,",
  "bv1:15.4_0": "dubhato passesu nimmitā.",
  "bv1:16.1_0": "Maṇimuttāvālikākiṇṇā,",
  "bv1:16.2_0": "nimmito ratanāmayo;",
  "bv1:16.3_0": "Obhāseti disā sabbā,",
  "bv1:16.4_0": "sataraṁsīva uggato.",
  "bv1:17.1_0": "Tasmiṁ caṅkamane dhīro,",
  "bv1:17.2_0": "dvattiṁsavaralakkhaṇo;",
  "bv1:17.3_0": "Virocamāno sambuddho,",
  "bv1:17.4_0": "caṅkame caṅkamī jino.",
  "bv1:18.1_0": "Dibbaṁ mandāravaṁ pupphaṁ,",
  "bv1:18.2_0": "padumaṁ pārichattakaṁ;",
  "bv1:18.3_0": "Caṅkamane okiranti,",
  "bv1:18.4_0": "sabbe devā samāgatā.",
  "bv1:19.1_0": "Passanti taṁ devasaṅghā,",
  "bv1:19.2_0": "dasasahassī pamoditā;",
  "bv1:19.3_0": "Namassamānā nipatanti,",
  "bv1:19.4_0": "tuṭṭhahaṭṭhā pamoditā.",
  "bv1:20.1_0": "Tāvatiṁsā ca yāmā ca,",
  "bv1:20.2_0": "tusitā cāpi devatā;",
  "bv1:20.3_0": "Nimmānaratino devā,",
  "bv1:20.4_0": "ye devā vasavattino;",
  "bv1:20.5_0": "Udaggacittā sumanā,",
  "bv1:20.6_0": "passanti lokanāyakaṁ.",
  "bv1:21.1_0": "Sadevagandhabbamanussarakkhasā,",
  "bv1:21.2_0": "Nāgā supaṇṇā atha vāpi kinnarā;",
  "bv1:21.3_0": "Passanti taṁ lokahitānukampakaṁ,",
  "bv1:21.4_0": "Nabheva accuggatacandamaṇḍalaṁ.",
  "bv1:22.1_0": "Ābhassarā subhakiṇhā,",
  "bv1:22.2_0": "Vehapphalā akaniṭṭhā ca devatā;",
  "bv1:22.3_0": "Susuddhasukkavatthavasanā,",
  "bv1:22.4_0": "Tiṭṭhanti pañjalīkatā.",
  "bv1:23.1_0": "Muñcanti pupphaṁ pana pañcavaṇṇikaṁ,",
  "bv1:23.2_0": "Mandāravaṁ candanacuṇṇamissitaṁ;",
  "bv1:23.3_0": "Bhamenti celāni ca ambare tadā,",
  "bv1:23.4_0": "“Aho jino lokahitānukampako.",
  "bv1:24.1_0": "Tuvaṁ satthā ca ketū ca,",
  "bv1:24.2_0": "dhajo yūpo ca pāṇinaṁ;",
  "bv1:24.3_0": "Parāyaṇo patiṭṭhā ca,",
  "bv1:24.4_0": "dīpo ca dvipaduttamo”.",
  "bv1:25.1_0": "Dasasahassīlokadhātuyā,",
  "bv1:25.2_0": "Devatāyo mahiddhikā;",
  "bv1:25.3_0": "Parivāretvā namassanti,",
  "bv1:25.4_0": "Tuṭṭhahaṭṭhā pamoditā.",
  "bv1:26.1_0": "Devatā devakaññā ca,",
  "bv1:26.2_0": "pasannā tuṭṭhamānasā;",
  "bv1:26.3_0": "Pañcavaṇṇikapupphehi,",
  "bv1:26.4_0": "pūjayanti narāsabhaṁ.",
  "bv1:27.1_0": "Passanti taṁ devasaṅghā,",
  "bv1:27.2_0": "pasannā tuṭṭhamānasā;",
  "bv1:27.3_0": "Pañcavaṇṇikapupphehi,",
  "bv1:27.4_0": "pūjayanti narāsabhaṁ.",
  "bv1:28.1_0": "“Aho acchariyaṁ loke,",
  "bv1:28.2_0": "abbhutaṁ lomahaṁsanaṁ;",
  "bv1:28.3_0": "Na medisaṁ bhūtapubbaṁ,",
  "bv1:28.4_0": "accheraṁ lomahaṁsanaṁ”.",
  "bv1:29.1_0": "Sakasakamhi bhavane,",
  "bv1:29.2_0": "nisīditvāna devatā;",
  "bv1:29.3_0": "Hasanti tā mahāhasitaṁ,",
  "bv1:29.4_0": "disvānaccherakaṁ nabhe.",
  "bv1:30.1_0": "Ākāsaṭṭhā ca bhūmaṭṭhā,",
  "bv1:30.2_0": "tiṇapanthanivāsino;",
  "bv1:30.3_0": "Katañjalī namassanti,",
  "bv1:30.4_0": "tuṭṭhahaṭṭhā pamoditā.",
  "bv1:31.1_0": "Yepi dīghāyukā nāgā,",
  "bv1:31.2_0": "puññavanto mahiddhikā;",
  "bv1:31.3_0": "Pamoditā namassanti,",
  "bv1:31.4_0": "pūjayanti naruttamaṁ.",
  "bv1:32.1_0": "Saṅgītiyo pavattenti,",
  "bv1:32.2_0": "ambare anilañjase;",
  "bv1:32.3_0": "Cammanaddhāni vādenti,",
  "bv1:32.4_0": "disvānaccherakaṁ nabhe.",
  "bv1:33.1_0": "Saṅkhā ca paṇavā ceva,",
  "bv1:33.2_0": "athopi ḍiṇḍimā bahū;",
  "bv1:33.3_0": "Antalikkhasmiṁ vajjanti,",
  "bv1:33.4_0": "disvānaccherakaṁ nabhe.",
  "bv1:34.1_0": "“Abbhuto vata no ajja,",
  "bv1:34.2_0": "uppajji lomahaṁsano;",
  "bv1:34.3_0": "Dhuvamatthasiddhiṁ labhāma,",
  "bv1:34.4_0": "khaṇo no paṭipādito”.",
  "bv1:35.1_0": "Buddhoti tesaṁ sutvāna,",
  "bv1:35.2_0": "pīti uppajji tāvade;",
  "bv1:35.3_0": "Buddho buddhoti kathayantā,",
  "bv1:35.4_0": "tiṭṭhanti pañjalīkatā.",
  "bv1:36.1_0": "Hiṅkārā sādhukārā ca,",
  "bv1:36.2_0": "ukkuṭṭhi sampahaṁsanaṁ;",
  "bv1:36.3_0": "Pajā ca vividhā gagane,",
  "bv1:36.4_0": "vattenti pañjalīkatā.",
  "bv1:37.1_0": "Gāyanti seḷenti ca vādayanti ca,",
  "bv1:37.2_0": "Bhujāni pothenti ca naccayanti ca;",
  "bv1:37.3_0": "Muñcanti pupphaṁ pana pañcavaṇṇikaṁ,",
  "bv1:37.4_0": "Mandāravaṁ candanacuṇṇamissitaṁ.",
  "bv1:38.1_0": "“Yathā tuyhaṁ mahāvīra,",
  "bv1:38.2_0": "pādesu cakkalakkhaṇaṁ;",
  "bv1:38.3_0": "Dhajavajirapaṭākā,",
  "bv1:38.4_0": "vaḍḍhamānaṅkusācitaṁ.",
  "bv1:39.1_0": "Rūpe sīle samādhimhi,",
  "bv1:39.2_0": "paññāya ca asādiso;",
  "bv1:39.3_0": "Vimuttiyā asamasamo,",
  "bv1:39.4_0": "dhammacakkappavattane.",
  "bv1:40.1_0": "Dasanāgabalaṁ kāye,",
  "bv1:40.2_0": "tuyhaṁ pākatikaṁ balaṁ;",
  "bv1:40.3_0": "Iddhibalena asamo,",
  "bv1:40.4_0": "dhammacakkappavattane.",
  "bv1:41.1_0": "Evaṁ sabbaguṇūpetaṁ,",
  "bv1:41.2_0": "sabbaṅgasamupāgataṁ;",
  "bv1:41.3_0": "Mahāmuniṁ kāruṇikaṁ,",
  "bv1:41.4_0": "lokanāthaṁ namassatha.",
  "bv1:42.1_0": "Abhivādanaṁ thomanañca,",
  "bv1:42.2_0": "vandanañca pasaṁsanaṁ;",
  "bv1:42.3_0": "Namassanañca pūjañca,",
  "bv1:42.4_0": "sabbaṁ arahasī tuvaṁ.",
  "bv1:43.1_0": "Ye keci loke vandaneyyā,",
  "bv1:43.2_0": "vandanaṁ arahanti ye;",
  "bv1:43.3_0": "Sabbaseṭṭho mahāvīra,",
  "bv1:43.4_0": "sadiso te na vijjati.",
  "bv1:44.1_0": "Sāriputto mahāpañño,",
  "bv1:44.2_0": "samādhijjhānakovido;",
  "bv1:44.3_0": "Gijjhakūṭe ṭhitoyeva,",
  "bv1:44.4_0": "passati lokanāyakaṁ.",
  "bv1:45.1_0": "Suphullaṁ sālarājaṁva,",
  "bv1:45.2_0": "candaṁva gagane yathā;",
  "bv1:45.3_0": "Majjhanhikeva sūriyaṁ,",
  "bv1:45.4_0": "olokesi narāsabhaṁ.",
  "bv1:46.1_0": "Jalantaṁ dīparukkhaṁva,",
  "bv1:46.2_0": "Taruṇasūriyaṁva uggataṁ;",
  "bv1:46.3_0": "Byāmappabhānurañjitaṁ,",
  "bv1:46.4_0": "Dhīraṁ passati lokanāyakaṁ.",
  "bv1:47.1_0": "Pañcannaṁ bhikkhusatānaṁ,",
  "bv1:47.2_0": "katakiccāna tādinaṁ;",
  "bv1:47.3_0": "Khīṇāsavānaṁ vimalānaṁ,",
  "bv1:47.4_0": "khaṇena sannipātayi.",
  "bv1:48.1_0": "Lokappasādanaṁ nāma,",
  "bv1:48.2_0": "pāṭihīraṁ nidassayi;",
  "bv1:48.3_0": "Amhepi tattha gantvāna,",
  "bv1:48.4_0": "vandissāma mayaṁ jinaṁ.",
  "bv1:49.1_0": "Etha sabbe gamissāma,",
  "bv1:49.2_0": "pucchissāma mayaṁ jinaṁ;",
  "bv1:49.3_0": "Kaṅkhaṁ vinodayissāma,",
  "bv1:49.4_0": "passitvā lokanāyakaṁ”.",
  "bv1:50.1_0": "Sādhūti te paṭissutvā,",
  "bv1:50.2_0": "nipakā saṁvutindriyā;",
  "bv1:50.3_0": "Pattacīvaramādāya,",
  "bv1:50.4_0": "taramānā upāgamuṁ.",
  "bv1:51.1_0": "Khīṇāsavehi vimalehi,",
  "bv1:51.2_0": "dantehi uttame dame;",
  "bv1:51.3_0": "Sāriputto mahāpañño,",
  "bv1:51.4_0": "iddhiyā upasaṅkami.",
  "bv1:52.1_0": "Tehi bhikkhūhi parivuto,",
  "bv1:52.2_0": "sāriputto mahāgaṇī;",
  "bv1:52.3_0": "Laḷanto devova gagane,",
  "bv1:52.4_0": "iddhiyā upasaṅkami.",
  "bv1:53.1_0": "Ukkāsitañca khipitaṁ,",
  "bv1:53.2_0": "ajjhupekkhiya subbatā;",
  "bv1:53.3_0": "Sagāravā sappatissā,",
  "bv1:53.4_0": "sambuddhaṁ upasaṅkamuṁ.",
  "bv1:54.1_0": "Upasaṅkamitvā passanti,",
  "bv1:54.2_0": "sayambhuṁ lokanāyakaṁ;",
  "bv1:54.3_0": "Nabhe accuggataṁ dhīraṁ,",
  "bv1:54.4_0": "candaṁva gagane yathā.",
  "bv1:55.1_0": "Jalantaṁ dīparukkhaṁva,",
  "bv1:55.2_0": "vijjuṁva gagane yathā;",
  "bv1:55.3_0": "Majjhanhikeva sūriyaṁ,",
  "bv1:55.4_0": "passanti lokanāyakaṁ.",
  "bv1:56.1_0": "Pañcabhikkhusatā sabbe,",
  "bv1:56.2_0": "passanti lokanāyakaṁ;",
  "bv1:56.3_0": "Rahadamiva vippasannaṁ,",
  "bv1:56.4_0": "suphullaṁ padumaṁ yathā.",
  "bv1:57.1_0": "Añjaliṁ paggahetvāna,",
  "bv1:57.2_0": "tuṭṭhahaṭṭhā pamoditā;",
  "bv1:57.3_0": "Namassamānā nipatanti,",
  "bv1:57.4_0": "satthuno cakkalakkhaṇe.",
  "bv1:58.1_0": "Sāriputto mahāpañño,",
  "bv1:58.2_0": "koraṇḍasamasādiso;",
  "bv1:58.3_0": "Samādhijjhānakusalo,",
  "bv1:58.4_0": "vandate lokanāyakaṁ.",
  "bv1:59.1_0": "Gajjitā kālameghova,",
  "bv1:59.2_0": "nīluppalasamasādiso;",
  "bv1:59.3_0": "Iddhibalena asamo,",
  "bv1:59.4_0": "moggallāno mahiddhiko.",
  "bv1:60.1_0": "Mahākassapopi ca thero,",
  "bv1:60.2_0": "uttattakanakasannibho;",
  "bv1:60.3_0": "Dhutaguṇe agganikkhitto,",
  "bv1:60.4_0": "thomito satthuvaṇṇito.",
  "bv1:61.1_0": "Dibbacakkhūnaṁ yo aggo,",
  "bv1:61.2_0": "anuruddho mahāgaṇī;",
  "bv1:61.3_0": "Ñātiseṭṭho bhagavato,",
  "bv1:61.4_0": "avidūreva tiṭṭhati.",
  "bv1:62.1_0": "Āpattianāpattiyā,",
  "bv1:62.2_0": "satekicchāya kovido;",
  "bv1:62.3_0": "Vinaye agganikkhitto,",
  "bv1:62.4_0": "upāli satthuvaṇṇito.",
  "bv1:63.1_0": "Sukhumanipuṇatthapaṭividdho,",
  "bv1:63.2_0": "Kathikānaṁ pavaro gaṇī;",
  "bv1:63.3_0": "Isi mantāniyā putto,",
  "bv1:63.4_0": "Puṇṇo nāmāti vissuto.",
  "bv1:64.1_0": "Etesaṁ cittamaññāya,",
  "bv1:64.2_0": "opammakusalo muni;",
  "bv1:64.3_0": "Kaṅkhacchedo mahāvīro,",
  "bv1:64.4_0": "kathesi attano guṇaṁ.",
  "bv1:65.1_0": "“Cattāro te asaṅkheyyā,",
  "bv1:65.2_0": "koṭi yesaṁ na nāyati;",
  "bv1:65.3_0": "Sattakāyo ca ākāso,",
  "bv1:65.4_0": "cakkavāḷā canantakā;",
  "bv1:65.5_0": "Buddhañāṇaṁ appameyyaṁ,",
  "bv1:65.6_0": "na sakkā ete vijānituṁ.",
  "bv1:66.1_0": "Kimetaṁ acchariyaṁ loke,",
  "bv1:66.2_0": "yaṁ me iddhivikubbanaṁ;",
  "bv1:66.3_0": "Aññe bahū acchariyā,",
  "bv1:66.4_0": "abbhutā lomahaṁsanā.",
  "bv1:67.1_0": "Yadāhaṁ tusite kāye,",
  "bv1:67.2_0": "santusito nāmahaṁ tadā;",
  "bv1:67.3_0": "Dasasahassī samāgamma,",
  "bv1:67.4_0": "yācanti pañjalī mamaṁ.",
  "bv1:68.1_0": "‘Kālo kho te mahāvīra,",
  "bv1:68.2_0": "uppajja mātukucchiyaṁ;",
  "bv1:68.3_0": "Sadevakaṁ tārayanto,",
  "bv1:68.4_0": "bujjhassu amataṁ padaṁ’.",
  "bv1:69.1_0": "Tusitā kāyā cavitvāna,",
  "bv1:69.2_0": "yadā okkami kucchiyaṁ;",
  "bv1:69.3_0": "Dasasahassīlokadhātu,",
  "bv1:69.4_0": "kampittha dharaṇī tadā.",
  "bv1:70.1_0": "Yadāhaṁ mātukucchito,",
  "bv1:70.2_0": "sampajānova nikkhamiṁ;",
  "bv1:70.3_0": "Sādhukāraṁ pavattenti,",
  "bv1:70.4_0": "dasasahassī pakampatha.",
  "bv1:71.1_0": "Okkantiṁ me samo natthi,",
  "bv1:71.2_0": "jātito abhinikkhame;",
  "bv1:71.3_0": "Sambodhiyaṁ ahaṁ seṭṭho,",
  "bv1:71.4_0": "dhammacakkappavattane.",
  "bv1:72.1_0": "Aho acchariyaṁ loke,",
  "bv1:72.2_0": "buddhānaṁ guṇamahantatā;",
  "bv1:72.3_0": "Dasasahassīlokadhātu,",
  "bv1:72.4_0": "chappakāraṁ pakampatha;",
  "bv1:72.5_0": "Obhāso ca mahā āsi,",
  "bv1:72.6_0": "accheraṁ lomahaṁsanaṁ”.",
  "bv1:73.1_0": "Bhagavā tamhi samaye,",
  "bv1:73.2_0": "lokajeṭṭho narāsabho;",
  "bv1:73.3_0": "Sadevakaṁ dassayanto,",
  "bv1:73.4_0": "iddhiyā caṅkamī jino.",
  "bv1:74.1_0": "Caṅkame caṅkamantova,",
  "bv1:74.2_0": "kathesi lokanāyako;",
  "bv1:74.3_0": "Antarā na nivatteti,",
  "bv1:74.4_0": "catuhatthe caṅkame yathā.",
  "bv1:75.1_0": "Sāriputto mahāpañño,",
  "bv1:75.2_0": "samādhijjhānakovido;",
  "bv1:75.3_0": "Paññāya pāramippatto,",
  "bv1:75.4_0": "pucchati lokanāyakaṁ.",
  "bv1:76.1_0": "“Kīdiso te mahāvīra,",
  "bv1:76.2_0": "abhinīhāro naruttama;",
  "bv1:76.3_0": "Kamhi kāle tayā dhīra,",
  "bv1:76.4_0": "patthitā bodhimuttamā.",
  "bv1:77.1_0": "Dānaṁ sīlañca nekkhammaṁ,",
  "bv1:77.2_0": "paññāvīriyañca kīdisaṁ;",
  "bv1:77.3_0": "Khantisaccamadhiṭṭhānaṁ,",
  "bv1:77.4_0": "mettupekkhā ca kīdisā.",
  "bv1:78.1_0": "Dasa pāramī tayā dhīra,",
  "bv1:78.2_0": "kīdisī lokanāyaka;",
  "bv1:78.3_0": "Kathaṁ upapāramī puṇṇā,",
  "bv1:78.4_0": "paramatthapāramī kathaṁ”.",
  "bv1:79.1_0": "Tassa puṭṭho viyākāsi,",
  "bv1:79.2_0": "karavīkamadhuragiro;",
  "bv1:79.3_0": "Nibbāpayanto hadayaṁ,",
  "bv1:79.4_0": "hāsayanto sadevakaṁ.",
  "bv1:80.1_0": "Atītabuddhānaṁ jinānaṁ desitaṁ,",
  "bv1:80.2_0": "Nikīlitaṁ buddhaparamparāgataṁ;",
  "bv1:80.3_0": "Pubbenivāsānugatāya buddhiyā,",
  "bv1:80.4_0": "Pakāsayī lokahitaṁ sadevake.",
  "bv1:81.1_0": "“Pītipāmojjajananaṁ,",
  "bv1:81.2_0": "sokasallavinodanaṁ;",
  "bv1:81.3_0": "Sabbasampattipaṭilābhaṁ,",
  "bv1:81.4_0": "cittīkatvā suṇātha me.",
  "bv1:82.1_0": "Madanimmadanaṁ sokanudaṁ,",
  "bv1:82.2_0": "Saṁsāraparimocanaṁ;",
  "bv1:82.3_0": "Sabbadukkhakkhayaṁ maggaṁ,",
  "bv1:82.4_0": "Sakkaccaṁ paṭipajjathā”ti.",
  "bv1:83.1_0": "Ratanacaṅkamanakaṇḍo niṭṭhito."
}