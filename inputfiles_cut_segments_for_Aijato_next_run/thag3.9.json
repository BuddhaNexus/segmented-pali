{
  "thag3.9:0.1_0": "Theragāthā",
  "thag3.9:0.2_0": "Tikanipāta",
  "thag3.9:0.3_0": "Paṭhamavagga",
  "thag3.9:0.4_0": "3.9. Yasojattheragāthā",
  "thag3.9:1.1_0": "“Kālapabbaṅgasaṅkāso,",
  "thag3.9:1.2_0": "kiso dhamanisanthato;",
  "thag3.9:1.3_0": "Mattaññū annapānamhi,",
  "thag3.9:1.4_0": "adīnamānaso naro”.",
  "thag3.9:2.1_0": "“Phuṭṭho ḍaṁsehi makasehi,",
  "thag3.9:2.2_0": "araññasmiṁ brahāvane;",
  "thag3.9:2.3_0": "Nāgo saṅgāmasīseva,",
  "thag3.9:2.4_0": "sato tatrādhivāsaye.",
  "thag3.9:3.1_0": "Yathā brahmā tathā eko,",
  "thag3.9:3.2_0": "yathā devo tathā duve;",
  "thag3.9:3.3_0": "Yathā gāmo tathā tayo,",
  "thag3.9:3.4_0": "kolāhalaṁ tatuttarin”ti.",
  "thag3.9:3.5_0": "… Yasojo thero …."
}