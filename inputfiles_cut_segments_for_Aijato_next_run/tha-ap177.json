{
  "tha-ap177:0.1_0": "Therāpadāna",
  "tha-ap177:0.2_0": "Kumudavagga",
  "tha-ap177:0.3_0": "5. Sīhāsanadāyakattheraapadāna",
  "tha-ap177:1.1_0": "“Nibbute lokanāthamhi,",
  "tha-ap177:1.2_0": "padumuttaranāyake;",
  "tha-ap177:1.3_0": "Pasannacitto sumano,",
  "tha-ap177:1.4_0": "sīhāsanamadāsahaṁ.",
  "tha-ap177:2.1_0": "Bahūhi gandhamālehi,",
  "tha-ap177:2.2_0": "diṭṭhadhammasukhāvahe;",
  "tha-ap177:2.3_0": "Tattha pūjañca katvāna,",
  "tha-ap177:2.4_0": "nibbāyati bahujjano.",
  "tha-ap177:3.1_0": "Pasannacitto sumano,",
  "tha-ap177:3.2_0": "vanditvā bodhimuttamaṁ;",
  "tha-ap177:3.3_0": "Kappānaṁ satasahassaṁ,",
  "tha-ap177:3.4_0": "duggatiṁ nupapajjahaṁ.",
  "tha-ap177:4.1_0": "Pannarasasahassamhi,",
  "tha-ap177:4.2_0": "kappānaṁ aṭṭha āsu te;",
  "tha-ap177:4.3_0": "Siluccayasanāmā ca,",
  "tha-ap177:4.4_0": "rājāno cakkavattino.",
  "tha-ap177:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap177:5.2_0": "…pe…",
  "tha-ap177:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap177:6.1_0": "Itthaṁ sudaṁ āyasmā sīhāsanadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap177:7.1_0": "Sīhāsanadāyakattherassāpadānaṁ pañcamaṁ."
}