{
  "tika-vin10t6:0_0": "Mahāvibhaṅgasaṅgahakathā",
  "tika-vin10t6:1_0": "4.",
  "tika-vin10t6:2_0": "Methunaṁ paṭisevanto, kati āpattiyo phuse;",
  "tika-vin10t6:3_0": "Methunaṁ paṭisevanto, tisso āpattiyo phuse.",
  "tika-vin10t6:4_0": "5.",
  "tika-vin10t6:5_0": "Bhave pārājikaṁ khette, yebhuyyakkhāyite pana;",
  "tika-vin10t6:6_0": "Thullaccayaṁ mukhe vaṭṭa-kate vuttaṁ tu dukkaṭaṁ.",
  "tika-vin10t6:7_0": "6.",
  "tika-vin10t6:8_0": "Adinnaṁ ādiyanto yo;",
  "tika-vin10t6:9_0": "Kati āpattiyo phuse;",
  "tika-vin10t6:10_0": "Adinnaṁ ādiyanto so;",
  "tika-vin10t6:11_0": "Tisso āpattiyo phuse.",
  "tika-vin10t6:12_0": "7.",
  "tika-vin10t6:13_0": "Pañcamāsagghane vāpi, adhike vā parājayo;",
  "tika-vin10t6:14_0": "Māse vā dukkaṭaṁ ūne, majjhe thullaccayaṁ tato.",
  "tika-vin10t6:15_0": "8.",
  "tika-vin10t6:16_0": "Manussajātiṁ mārento;",
  "tika-vin10t6:17_0": "Kati āpattiyo phuse;",
  "tika-vin10t6:18_0": "Manussajātiṁ mārento;",
  "tika-vin10t6:19_0": "Tisso āpattiyo phuse.",
  "tika-vin10t6:20_0": "9.",
  "tika-vin10t6:21_0": "Manussamuddissopātaṁ, khaṇane dukkaṭaṁ siyā;",
  "tika-vin10t6:22_0": "Dukkhe thullaccayaṁ jāte, mate pārājikaṁ siyā.",
  "tika-vin10t6:23_0": "10.",
  "tika-vin10t6:24_0": "Asantaṁ uttariṁ dhammaṁ, vadamattūpanāyikaṁ;",
  "tika-vin10t6:25_0": "Kati āpajjatāpattī? Tisso āpattiyo phuse.",
  "tika-vin10t6:26_0": "11.",
  "tika-vin10t6:27_0": "Asantaṁ uttariṁ dhammaṁ, bhaṇantassa parājayo;",
  "tika-vin10t6:28_0": "Thullaccayaṁ pariyāye, ñāte, no ce tu dukkaṭaṁ.",
  "tika-vin10t6:29_0": "Pārājikakathā.",
  "tika-vin10t6:30_0": "12.",
  "tika-vin10t6:31_0": "Bhaṇa sukkaṁ vimocento;",
  "tika-vin10t6:32_0": "Kati āpattiyo phuse;",
  "tika-vin10t6:33_0": "Suṇa sukkaṁ vimocento;",
  "tika-vin10t6:34_0": "Tisso āpattiyo phuse.",
  "tika-vin10t6:35_0": "13.",
  "tika-vin10t6:36_0": "Garukaṁ yadi ceteti, upakkamati muccati;",
  "tika-vin10t6:37_0": "Dvaṅge thullaccayaṁ vuttaṁ, payoge dukkaṭaṁ siyā.",
  "tika-vin10t6:38_0": "14.",
  "tika-vin10t6:39_0": "Ito paṭṭhāya muñcitvā, pañhāpucchanamattakaṁ;",
  "tika-vin10t6:40_0": "Vissajjanavaseneva, hoti atthavinicchayo.",
  "tika-vin10t6:41_0": "15.",
  "tika-vin10t6:42_0": "Itthiyā kāyasaṁsagge, tisso āpattiyo phuse;",
  "tika-vin10t6:43_0": "Āmasantassa kāyena, kāyaṁ tu garukaṁ siyā.",
  "tika-vin10t6:44_0": "16.",
  "tika-vin10t6:45_0": "Kāyena kāyabaddhaṁ tu, phusaṁ thullaccayaṁ phuse;",
  "tika-vin10t6:46_0": "Paṭibaddhena kāyena, paṭibaddhe tu dukkaṭaṁ.",
  "tika-vin10t6:47_0": "17.",
  "tika-vin10t6:48_0": "Itthiṁ duṭṭhullavācāhi, tisso obhāsato siyuṁ;",
  "tika-vin10t6:49_0": "Vaṇṇāvaṇṇaṁ vadaṁ dvinnaṁ, maggānaṁ garukaṁ phuse.",
  "tika-vin10t6:50_0": "18.",
  "tika-vin10t6:51_0": "Vaṇṇādibhaññe ādissa, ubbhajāṇumadhakkhakaṁ;",
  "tika-vin10t6:52_0": "Hoti thullaccayaṁ, kāya-paṭibaddhe tu dukkaṭaṁ.",
  "tika-vin10t6:53_0": "19.",
  "tika-vin10t6:54_0": "Attakāmacariyāya, vadato vaṇṇamitthiyā;",
  "tika-vin10t6:55_0": "Santike garukaṁ hoti, sace jānāti sā pana.",
  "tika-vin10t6:56_0": "20.",
  "tika-vin10t6:57_0": "Santike paṇḍakassāpi, tassa thullaccayaṁ siyā;",
  "tika-vin10t6:58_0": "Tiracchānagatassāpi, santike dukkaṭaṁ mataṁ.",
  "tika-vin10t6:59_0": "21.",
  "tika-vin10t6:60_0": "Paṭiggaṇhanavīmaṁsā, paccāharaṇakattike;",
  "tika-vin10t6:61_0": "Sañcarittaṁ samāpanne, garukaṁ niddise budho.",
  "tika-vin10t6:62_0": "22.",
  "tika-vin10t6:63_0": "Tassa dvaṅgasamāyoge, hoti thullaccayaṁ tathā;",
  "tika-vin10t6:64_0": "Aṅge sati panekasmiṁ, hoti āpatti dukkaṭaṁ.",
  "tika-vin10t6:65_0": "23.",
  "tika-vin10t6:66_0": "Saṁyācikāya ca kuṭiṁ;",
  "tika-vin10t6:67_0": "Vihārañca mahallakaṁ;",
  "tika-vin10t6:68_0": "Kārāpeti sace bhikkhu;",
  "tika-vin10t6:69_0": "Tisso āpattiyo phuse.",
  "tika-vin10t6:70_0": "24.",
  "tika-vin10t6:71_0": "Payoge dukkaṭaṁ vuttaṁ, ekapiṇḍe anāgate;",
  "tika-vin10t6:72_0": "Hoti thullaccayaṁ, tasmiṁ, piṇḍe garukamāgate.",
  "tika-vin10t6:73_0": "25.",
  "tika-vin10t6:74_0": "Pārājikena dhammena, bhikkhuṁ amūlakenidha;",
  "tika-vin10t6:75_0": "Anuddhaṁseti yo tassa, tisso āpattiyo siyuṁ.",
  "tika-vin10t6:76_0": "26.",
  "tika-vin10t6:77_0": "Okāsaṁ na ca kāretvā, hutvā cāvanacetano;",
  "tika-vin10t6:78_0": "Sace codeti saṅghādi-sesena saha dukkaṭaṁ.",
  "tika-vin10t6:79_0": "27.",
  "tika-vin10t6:80_0": "Okāsaṁ pana kāretvā, hutvā akkosacetano;",
  "tika-vin10t6:81_0": "Codeti omasavāde, pācittiṁ paridīpaye.",
  "tika-vin10t6:82_0": "28.",
  "tika-vin10t6:83_0": "Anantarasamānova, navame aññabhāgiye;",
  "tika-vin10t6:84_0": "Sabbo āpattibhedo hi, natthi kāci visesatā.",
  "tika-vin10t6:85_0": "29.",
  "tika-vin10t6:86_0": "Saṅghassa bhedako bhikkhu, yāvatatiyakaṁ pana;",
  "tika-vin10t6:87_0": "Samanubhāsanāyeva, gāhaṁ na paṭinissajaṁ.",
  "tika-vin10t6:88_0": "30.",
  "tika-vin10t6:89_0": "Ñattiyā dukkaṭaṁ, dvīhi, kammavācāhi thullataṁ;",
  "tika-vin10t6:90_0": "Kammavācāya osāne, āpatti garukaṁ siyā.",
  "tika-vin10t6:91_0": "31.",
  "tika-vin10t6:92_0": "Bhedānuvattake ceva, dubbace kuladūsake;",
  "tika-vin10t6:93_0": "Saṅghabhedakatulyova, hoti āpattinicchayo.",
  "tika-vin10t6:94_0": "Saṅghādisesakathā.",
  "tika-vin10t6:95_0": "32.",
  "tika-vin10t6:96_0": "Atikkamanto atirekacīvaraṁ;",
  "tika-vin10t6:97_0": "Dasāhamāpajjati ekameva;",
  "tika-vin10t6:98_0": "Nissaggipācittiyamekarattiṁ;",
  "tika-vin10t6:99_0": "Ticīvarenāpi vinā vasanto.",
  "tika-vin10t6:100_0": "33.",
  "tika-vin10t6:101_0": "Māsaṁ atikkamanto hi, gahetvā kālacīvaraṁ;",
  "tika-vin10t6:102_0": "Ekaṁ āpajjatāpattiṁ, nissaggiyamudīritaṁ.",
  "tika-vin10t6:103_0": "34.",
  "tika-vin10t6:104_0": "Aññātikāya yaṁ kiñci;",
  "tika-vin10t6:105_0": "Purāṇacīvaraṁ pana;",
  "tika-vin10t6:106_0": "Dhovāpeti sace tassa;",
  "tika-vin10t6:107_0": "Honti āpattiyo duve.",
  "tika-vin10t6:108_0": "35.",
  "tika-vin10t6:109_0": "Dhovāpeti payogasmiṁ, dukkaṭaṁ samudāhaṭaṁ;",
  "tika-vin10t6:110_0": "Nissaggiyāva pācitti, hoti dhovāpite pana.",
  "tika-vin10t6:111_0": "36.",
  "tika-vin10t6:112_0": "Aññātikāya hatthamhā, cīvaraṁ paṭigaṇhato;",
  "tika-vin10t6:113_0": "Gahaṇe dukkaṭaṁ vuttaṁ, pācitti gahite siyā.",
  "tika-vin10t6:114_0": "37.",
  "tika-vin10t6:115_0": "Aññātakaṁ gahapatiṁ, gahapatānimeva vā;",
  "tika-vin10t6:116_0": "Cīvaraṁ viññāpento dve, bhikkhu āpattiyo phuse.",
  "tika-vin10t6:117_0": "38.",
  "tika-vin10t6:118_0": "Viññāpeti payogasmiṁ, dukkaṭaṁ parikittitaṁ;",
  "tika-vin10t6:119_0": "Viññāpite ca nissaggi, pācitti pariyāputā.",
  "tika-vin10t6:120_0": "39.",
  "tika-vin10t6:121_0": "Bhikkhu cīvaramaññātiṁ, viññāpento taduttariṁ;",
  "tika-vin10t6:122_0": "Payoge dukkaṭaṁ, viññā-pite nissaggiyaṁ phuse.",
  "tika-vin10t6:123_0": "40.",
  "tika-vin10t6:124_0": "Aññātakaṁ kañci upāsakaṁ vā;",
  "tika-vin10t6:125_0": "Upāsikaṁ vā upasaṅkamitvā;",
  "tika-vin10t6:126_0": "Pubbeva hutvā pana appavārito;",
  "tika-vin10t6:127_0": "Vatthe vikappaṁ paṭipajjamāno.",
  "tika-vin10t6:128_0": "41.",
  "tika-vin10t6:129_0": "Duve āpajjatāpattī, payoge dukkaṭaṁ siyā;",
  "tika-vin10t6:130_0": "Vikappaṁ pana āpanne, nissaggiyamudīritaṁ.",
  "tika-vin10t6:131_0": "42.",
  "tika-vin10t6:132_0": "Aññātiṁ upasaṅkamma, pubbeyevappavārito;",
  "tika-vin10t6:133_0": "Vikappaṁ cīvare bhikkhu, āpajjanto duve phuse.",
  "tika-vin10t6:134_0": "43.",
  "tika-vin10t6:135_0": "Tathātirekatikkhattuṁ, codanāya ca bhikkhu ce;",
  "tika-vin10t6:136_0": "Gantvātirekachakkhattuṁ, ṭhānenapi ca cīvaraṁ.",
  "tika-vin10t6:137_0": "44.",
  "tika-vin10t6:138_0": "Nipphādeti sace tassa, honti āpattiyo duve;",
  "tika-vin10t6:139_0": "Payoge dukkaṭaṁ, tassa, lābhe nissaggiyaṁ siyā.",
  "tika-vin10t6:140_0": "Kathinavaggo paṭhamo.",
  "tika-vin10t6:141_0": "45.",
  "tika-vin10t6:142_0": "Dosā kosiyavaggassa, dvedveādīsu pañcasu;",
  "tika-vin10t6:143_0": "Payoge dukkaṭaṁ vuttaṁ, lābhe nissaggiyaṁ siyā.",
  "tika-vin10t6:144_0": "46.",
  "tika-vin10t6:145_0": "Gahetveḷakalomāni, tiyojanamatikkamaṁ;",
  "tika-vin10t6:146_0": "Dukkaṭaṁ paṭhame pāde, nissaggiṁ dutiye phuse.",
  "tika-vin10t6:147_0": "47.",
  "tika-vin10t6:148_0": "Bhikkhu bhikkhuniyaññāya, dhovāpeteḷalomakaṁ;",
  "tika-vin10t6:149_0": "Payoge dukkaṭaṁ, tassa, dhote nissaggiyaṁ siyā.",
  "tika-vin10t6:150_0": "48.",
  "tika-vin10t6:151_0": "Rūpiyaṁ paṭigaṇhanto, dve panāpattiyo phuse;",
  "tika-vin10t6:152_0": "Payoge dukkaṭaṁ vuttaṁ, nissaggi gahite siyā.",
  "tika-vin10t6:153_0": "49.",
  "tika-vin10t6:154_0": "Nānākāraṁ samāpajjaṁ, saṁvohārañca rūpiye;",
  "tika-vin10t6:155_0": "Samāpanne ca nissaggiṁ, payoge dukkaṭaṁ phuse.",
  "tika-vin10t6:156_0": "50.",
  "tika-vin10t6:157_0": "Nānappakārakaṁ bhikkhu, āpajje kayavikkayaṁ;",
  "tika-vin10t6:158_0": "Payoge dukkaṭaṁ, tasmiṁ, kate nissaggiyaṁ phuse.",
  "tika-vin10t6:159_0": "Kosiyavaggo dutiyo.",
  "tika-vin10t6:160_0": "51.",
  "tika-vin10t6:161_0": "Pattaṁ atikkamentassa, dasāhamatirekakaṁ;",
  "tika-vin10t6:162_0": "Tassa nissaggiyāpatti, hoti ekāva bhikkhuno.",
  "tika-vin10t6:163_0": "52.",
  "tika-vin10t6:164_0": "Apañcabandhane patte, vijjamānepi bhikkhuno;",
  "tika-vin10t6:165_0": "Aññaṁ pana navaṁ pattaṁ, cetāpeti sace pana.",
  "tika-vin10t6:166_0": "53.",
  "tika-vin10t6:167_0": "Dve panāpattiyo bhikkhu, āpajjati, na saṁsayo;",
  "tika-vin10t6:168_0": "Payoge dukkaṭaṁ, tassa, lābhe nissaggiyaṁ phuse.",
  "tika-vin10t6:169_0": "54.",
  "tika-vin10t6:170_0": "Paṭiggahetvā bhesajjaṁ, sattāhaṁ yo atikkame;",
  "tika-vin10t6:171_0": "Ekaṁ nissaggiyāpattiṁ, āpajjati hi so pana.",
  "tika-vin10t6:172_0": "55.",
  "tika-vin10t6:173_0": "Akāle pariyesanto, vassasāṭikacīvaraṁ;",
  "tika-vin10t6:174_0": "Payoge dukkaṭaṁ, tassa, lābhe nissaggiyaṁ phuse.",
  "tika-vin10t6:175_0": "56.",
  "tika-vin10t6:176_0": "Bhikkhuno cīvaraṁ datvā, acchindanto duve phuse;",
  "tika-vin10t6:177_0": "Payoge dukkaṭaṁ vuttaṁ, haṭe nissaggiyaṁ siyā.",
  "tika-vin10t6:178_0": "57.",
  "tika-vin10t6:179_0": "Viññāpetvā sayaṁ suttaṁ, tantavāyehi cīvaraṁ;",
  "tika-vin10t6:180_0": "Vāyāpeti sace bhikkhu, dve panāpattiyo phuse.",
  "tika-vin10t6:181_0": "58.",
  "tika-vin10t6:182_0": "Yo panaññātakasseva, tantavāye samecca ce;",
  "tika-vin10t6:183_0": "Vikappaṁ cīvare bhikkhu, āpajjaṁ appavārito.",
  "tika-vin10t6:184_0": "59.",
  "tika-vin10t6:185_0": "Dve panāpattiyo so hi, āpajjati, na saṁsayo;",
  "tika-vin10t6:186_0": "Payoge dukkaṭaṁ, tassa, lābhe nissaggiyaṁ siyā.",
  "tika-vin10t6:187_0": "60.",
  "tika-vin10t6:188_0": "Paṭiggahetvā acceka-saññitaṁ pana cīvaraṁ;",
  "tika-vin10t6:189_0": "Kālaṁ atikkamento taṁ, ekaṁ nissaggiyaṁ phuse.",
  "tika-vin10t6:190_0": "61.",
  "tika-vin10t6:191_0": "Tiṇṇamaññataraṁ vatthaṁ, nidahitvā gharedhikaṁ;",
  "tika-vin10t6:192_0": "Chārattato vinā tena, vasaṁ nissaggiyaṁ phuse.",
  "tika-vin10t6:193_0": "62.",
  "tika-vin10t6:194_0": "Jānaṁ pariṇataṁ lābhaṁ, saṅghikaṁ attano pana;",
  "tika-vin10t6:195_0": "Pariṇāmeti ce bhikkhu, dve panāpattiyo phuse.",
  "tika-vin10t6:196_0": "63.",
  "tika-vin10t6:197_0": "Payoge dukkaṭaṁ hoti, nissaggi pariṇāmite;",
  "tika-vin10t6:198_0": "Sabbattha appanāvāra-parihāni katā mayā.",
  "tika-vin10t6:199_0": "Pattavaggo tatiyo.",
  "tika-vin10t6:200_0": "Tiṁsanissaggiyakathā.",
  "tika-vin10t6:201_0": "64.",
  "tika-vin10t6:202_0": "Vadantassa musāvādaṁ, pañca āpattiyo siyuṁ;",
  "tika-vin10t6:203_0": "Manussuttaridhamme tu, abhūtasmiṁ parājayo.",
  "tika-vin10t6:204_0": "65.",
  "tika-vin10t6:205_0": "Codanāya garuṁ bhikkhuṁ, amūlantimavatthunā;",
  "tika-vin10t6:206_0": "Pariyāyavacane ñāte, thullaccayamudīritaṁ.",
  "tika-vin10t6:207_0": "66.",
  "tika-vin10t6:208_0": "No ce paṭivijānāti, dukkaṭaṁ samudāhaṭaṁ;",
  "tika-vin10t6:209_0": "Sampajānamusāvāde, pācitti paridīpitā.",
  "tika-vin10t6:210_0": "67.",
  "tika-vin10t6:211_0": "Āpattiyo duve vuttā, bhikkhussomasato pana;",
  "tika-vin10t6:212_0": "Pācitti upasampannaṁ, dukkaṭaṁ itaraṁ siyā.",
  "tika-vin10t6:213_0": "68.",
  "tika-vin10t6:214_0": "Pesuññaharaṇe dvepi, honti, pācittiyaṁ pana;",
  "tika-vin10t6:215_0": "Upasampannapesuññe, sese āpatti dukkaṭaṁ.",
  "tika-vin10t6:216_0": "69.",
  "tika-vin10t6:217_0": "Padasonupasampannaṁ, dhammaṁ vāceti ce duve;",
  "tika-vin10t6:218_0": "Payoge dukkaṭaṁ, pāde, pāde pācittiyaṁ siyā.",
  "tika-vin10t6:219_0": "70.",
  "tika-vin10t6:220_0": "Tirattānupasampanna-sahaseyyāya uttariṁ;",
  "tika-vin10t6:221_0": "Payoge dukkaṭaṁ vuttaṁ, panne pācittiyaṁ siyā.",
  "tika-vin10t6:222_0": "71.",
  "tika-vin10t6:223_0": "Kappeti mātugāmena, sahaseyyaṁ sace pana;",
  "tika-vin10t6:224_0": "Dve so āpajjatāpattī, rattiyaṁ dukkaṭādayo.",
  "tika-vin10t6:225_0": "72.",
  "tika-vin10t6:226_0": "Uddhaṁ chappañcavācāhi, dhammaṁ deseti itthiyā;",
  "tika-vin10t6:227_0": "Payoge dukkaṭaṁ, pāde, pāde pācittiyaṁ siyā.",
  "tika-vin10t6:228_0": "73.",
  "tika-vin10t6:229_0": "Bhūtaṁ anupasampanne, manussuttaridhammakaṁ;",
  "tika-vin10t6:230_0": "Āroceti sace tassa, honti dve dukkaṭādayo.",
  "tika-vin10t6:231_0": "74.",
  "tika-vin10t6:232_0": "Vadaṁ anupasampanne, duṭṭhullāpattimaññato;",
  "tika-vin10t6:233_0": "Payoge dukkaṭaṁ tassa, pācittārocite siyā.",
  "tika-vin10t6:234_0": "75.",
  "tika-vin10t6:235_0": "Pathaviṁ khaṇato tassa, payoge dukkaṭaṁ mataṁ;",
  "tika-vin10t6:236_0": "Pahāre ca pahāre ca, pācitti pariyāputā.",
  "tika-vin10t6:237_0": "Musāvādavaggo paṭhamo.",
  "tika-vin10t6:238_0": "76.",
  "tika-vin10t6:239_0": "Bhūtagāmaṁ tu pātento, dve panāpattiyo phuse;",
  "tika-vin10t6:240_0": "Payoge dukkaṭaṁ, tassa, pāte pācitti dīpitā.",
  "tika-vin10t6:241_0": "77.",
  "tika-vin10t6:242_0": "Aññenaññaṁ vadantassa, dve siyuṁ aññavādake;",
  "tika-vin10t6:243_0": "Aropite dukkaṭaṁ tu, hoti pācitti ropite.",
  "tika-vin10t6:244_0": "78.",
  "tika-vin10t6:245_0": "Ujjhāpento paraṁ bhikkhuṁ, dve panāpattiyo phuse;",
  "tika-vin10t6:246_0": "Payoge dukkaṭaṁ, ujjhā-pite pācittiyaṁ siyā.",
  "tika-vin10t6:247_0": "79.",
  "tika-vin10t6:248_0": "Ajjhokāse tu mañcādiṁ, santharitvāna saṅghikaṁ;",
  "tika-vin10t6:249_0": "Pakkamanto anāpucchā, āpattiṁ duvidhaṁ phuse.",
  "tika-vin10t6:250_0": "80.",
  "tika-vin10t6:251_0": "Leḍḍupāte atikkante, pādena paṭhamena tu;",
  "tika-vin10t6:252_0": "Dukkaṭaṁ, dutiyenāpi, pācitti paridīpaye.",
  "tika-vin10t6:253_0": "81.",
  "tika-vin10t6:254_0": "Vihāre saṅghike seyyaṁ, santharitvā anuddharaṁ;",
  "tika-vin10t6:255_0": "Anāpucchā pakkamanto, duvidhāpattiyo phuse.",
  "tika-vin10t6:256_0": "82.",
  "tika-vin10t6:257_0": "Parikkhepe atikkante, pādena paṭhamena tu;",
  "tika-vin10t6:258_0": "Dukkaṭaṁ pana uddiṭṭhaṁ, pācitti dutiyena tu.",
  "tika-vin10t6:259_0": "83.",
  "tika-vin10t6:260_0": "Vihāre saṅghike jānaṁ, pubbūpagatabhikkhukaṁ;",
  "tika-vin10t6:261_0": "Seyyaṁ kappayato honti, payoge dukkaṭādayo.",
  "tika-vin10t6:262_0": "84.",
  "tika-vin10t6:263_0": "Saṅghikā kupito bhikkhuṁ, nikkaḍḍhati vihārato;",
  "tika-vin10t6:264_0": "Payoge dukkaṭaṁ vuttaṁ, sesaṁ nikkaḍḍhite siyā.",
  "tika-vin10t6:265_0": "85.",
  "tika-vin10t6:266_0": "Vihāre saṅghike bhikkhu, vehāsakuṭiyūpari;",
  "tika-vin10t6:267_0": "Āhaccapādake sīdaṁ, phuse dve dukkaṭādayo.",
  "tika-vin10t6:268_0": "86.",
  "tika-vin10t6:269_0": "Adhiṭṭhitvā dvattipariyāye, uttarimpi adhiṭṭhato;",
  "tika-vin10t6:270_0": "Payoge dukkaṭaṁ hoti, pācitti panadhiṭṭhite.",
  "tika-vin10t6:271_0": "87.",
  "tika-vin10t6:272_0": "Jānaṁ sappāṇakaṁ toyaṁ, tiṇaṁ vā siñcato pana;",
  "tika-vin10t6:273_0": "Payoge dukkaṭaṁ hoti, sitte pācittiyaṁ siyā.",
  "tika-vin10t6:274_0": "Bhūtagāmavaggo dutiyo.",
  "tika-vin10t6:275_0": "88.",
  "tika-vin10t6:276_0": "Phuse bhikkhuniyo bhikkhu, ovadanto asammato;",
  "tika-vin10t6:277_0": "Payoge dukkaṭaṁ, tassa, pācittovadite siyā.",
  "tika-vin10t6:278_0": "89.",
  "tika-vin10t6:279_0": "Dutiye tatiye ceva, catutthepi ca sabbaso;",
  "tika-vin10t6:280_0": "Paṭhamena samānāva, āpattīnaṁ vibhāgatā.",
  "tika-vin10t6:281_0": "90.",
  "tika-vin10t6:282_0": "Cīvaraṁ bhikkhu aññāti-kāya dento duve phuse;",
  "tika-vin10t6:283_0": "Payoge dukkaṭaṁ vuttaṁ, dinne pācittiyaṁ siyā.",
  "tika-vin10t6:284_0": "91.",
  "tika-vin10t6:285_0": "Aññātikabhikkhuniyā, bhikkhu sibbeyya cīvaraṁ;",
  "tika-vin10t6:286_0": "Payoge dukkaṭaṁ tassa, pācitti pana sibbite.",
  "tika-vin10t6:287_0": "92.",
  "tika-vin10t6:288_0": "Addhānaññatra samayā, bhikkhu bhikkhuniyā saha;",
  "tika-vin10t6:289_0": "Saṁvidhāya tu gacchanto, phuse dve dukkaṭādayo.",
  "tika-vin10t6:290_0": "93.",
  "tika-vin10t6:291_0": "Nāvekaṁ abhirūhanto, bhikkhu bhikkhuniyā saha;",
  "tika-vin10t6:292_0": "Saṁvidhāya phuse dvepi, payoge dukkaṭādayo.",
  "tika-vin10t6:293_0": "94.",
  "tika-vin10t6:294_0": "Jānaṁ bhikkhuniyā piṇḍa-pātaṁ tu paripācitaṁ;",
  "tika-vin10t6:295_0": "Bhuñjanto duvidhāpatti-māpajjati, na saṁsayo.",
  "tika-vin10t6:296_0": "95.",
  "tika-vin10t6:297_0": "“Bhuñjissāmī”ti ce bhattaṁ, paṭiggaṇhāti dukkaṭaṁ;",
  "tika-vin10t6:298_0": "Ajjhohārapayogesu, pācitti paridīpitā.",
  "tika-vin10t6:299_0": "96.",
  "tika-vin10t6:300_0": "Bhikkhu bhikkhuniyā saddhiṁ, nisajjaṁ tu raho pana;",
  "tika-vin10t6:301_0": "Kappento hi phuse dvepi, payoge dukkaṭādayo.",
  "tika-vin10t6:302_0": "Ovādavaggo tatiyo.",
  "tika-vin10t6:303_0": "97.",
  "tika-vin10t6:304_0": "Taduttariṁ āvasatha-piṇḍaṁ tu paribhuñjato;",
  "tika-vin10t6:305_0": "Anantarassa vaggassa, navamena samo nayo.",
  "tika-vin10t6:306_0": "98.",
  "tika-vin10t6:307_0": "Dutiye tatiye cāpi, viseso natthi kocipi;",
  "tika-vin10t6:308_0": "Anantarasamānāva, āpattīnaṁ vibhāgatā.",
  "tika-vin10t6:309_0": "99.",
  "tika-vin10t6:310_0": "Dvattipatte gahetvāna, gaṇhato hi taduttariṁ;",
  "tika-vin10t6:311_0": "Payoge dukkaṭaṁ vuttaṁ, pācitti gahite siyā.",
  "tika-vin10t6:312_0": "100.",
  "tika-vin10t6:313_0": "Pañcame paṭhameneva, samo āpattinicchayo;",
  "tika-vin10t6:314_0": "Chaṭṭhe anatirittena, bhuttāviṁ tu pavāritaṁ.",
  "tika-vin10t6:315_0": "101.",
  "tika-vin10t6:316_0": "Abhihaṭṭhuṁ pavārento, dve panāpattiyo phuse;",
  "tika-vin10t6:317_0": "Vacanena ca tasseva, “bhuñjissāmī”ti gaṇhati.",
  "tika-vin10t6:318_0": "102.",
  "tika-vin10t6:319_0": "Gahaṇe dukkaṭaṁ tassa, piṭake samudāhaṭaṁ;",
  "tika-vin10t6:320_0": "Bhojanassa panosāne, pācitti pariyāputā.",
  "tika-vin10t6:321_0": "103.",
  "tika-vin10t6:322_0": "Sattame aṭṭhame ceva, navame dasamepi ca;",
  "tika-vin10t6:323_0": "Paṭhamena samānāva, āpattīnaṁ vibhāgatā.",
  "tika-vin10t6:324_0": "Bhojanavaggo catuttho.",
  "tika-vin10t6:325_0": "104.",
  "tika-vin10t6:326_0": "Acelakādino dento, sahatthā bhojanādikaṁ;",
  "tika-vin10t6:327_0": "Payoge dukkaṭaṁ patto, dinne pācittiyaṁ phuse.",
  "tika-vin10t6:328_0": "105.",
  "tika-vin10t6:329_0": "Dāpetvā vā adāpetvā, uyyojento duve phuse;",
  "tika-vin10t6:330_0": "Payoge dukkaṭaṁ, tasmiṁ, pācittuyyojite siyā.",
  "tika-vin10t6:331_0": "106.",
  "tika-vin10t6:332_0": "Nisajjaṁ bhikkhu kappento, kule pana sabhojane;",
  "tika-vin10t6:333_0": "Āpattiyo phuse dvepi, payoge dukkaṭādayo.",
  "tika-vin10t6:334_0": "107.",
  "tika-vin10t6:335_0": "Catutthe pañcame vāpi, viseso natthi kocipi;",
  "tika-vin10t6:336_0": "Tatiyena samānāva, āpattigaṇanā siyā.",
  "tika-vin10t6:337_0": "108.",
  "tika-vin10t6:338_0": "Santaṁ bhikkhuṁ anāpucchā, sabhatto ca nimantito;",
  "tika-vin10t6:339_0": "Kulesu pana cārittaṁ, āpajjanto duve phuse.",
  "tika-vin10t6:340_0": "109.",
  "tika-vin10t6:341_0": "Paṭhamena ca pādena, ummārātikkame pana;",
  "tika-vin10t6:342_0": "Dukkaṭaṁ piṭake vuttaṁ, pācitti dutiyena tu.",
  "tika-vin10t6:343_0": "110.",
  "tika-vin10t6:344_0": "Taduttariṁ tu bhesajjaṁ, viññāpento duve phuse;",
  "tika-vin10t6:345_0": "Payoge dukkaṭaṁ, viññā-pite pācittiyaṁ siyā.",
  "tika-vin10t6:346_0": "111.",
  "tika-vin10t6:347_0": "Uyyuttaṁ dassanatthāya, gacchanto dve phuse balaṁ;",
  "tika-vin10t6:348_0": "Gacchato dukkaṭaṁ vuttaṁ, hoti pācitti passato.",
  "tika-vin10t6:349_0": "112.",
  "tika-vin10t6:350_0": "Atirekatirattaṁ tu, senāya vasato duve;",
  "tika-vin10t6:351_0": "Payoge dukkaṭaṁ vuttaṁ, pācitti vasite siyā.",
  "tika-vin10t6:352_0": "113.",
  "tika-vin10t6:353_0": "Uyyodhikaṁ tu gacchanto, dve panāpattiyo phuse;",
  "tika-vin10t6:354_0": "Gacchanto dukkaṭaṁ vuttaṁ, hoti pācitti passato.",
  "tika-vin10t6:355_0": "Acelakavaggo pañcamo.",
  "tika-vin10t6:356_0": "114.",
  "tika-vin10t6:357_0": "Suraṁ vā pana mereyyaṁ, pivanto dve phuse muni;",
  "tika-vin10t6:358_0": "Gaṇhato dukkaṭaṁ pātuṁ, pīte pācittiyaṁ siyā.",
  "tika-vin10t6:359_0": "115.",
  "tika-vin10t6:360_0": "Bhikkhaṅgulipatodena, hāsento dve phuse have;",
  "tika-vin10t6:361_0": "Payoge dukkaṭaṁ tassa, pācitti hasite siyā.",
  "tika-vin10t6:362_0": "116.",
  "tika-vin10t6:363_0": "Kīḷanto udake bhikkhu, dve panāpattiyo phuse;",
  "tika-vin10t6:364_0": "Dukkaṭaṁ gopphakā heṭṭhā, pācittuparigopphake.",
  "tika-vin10t6:365_0": "117.",
  "tika-vin10t6:366_0": "Yo panādariyaṁ bhikkhu, karonto dve phuse have;",
  "tika-vin10t6:367_0": "Payoge dukkaṭaṁ vuttaṁ, kate pācittiyaṁ siyā.",
  "tika-vin10t6:368_0": "118.",
  "tika-vin10t6:369_0": "Bhiṁsāpento have bhikkhu, dve panāpattiyo phuse;",
  "tika-vin10t6:370_0": "Payoge dukkaṭaṁ, bhiṁsā-pite pācittiyaṁ siyā.",
  "tika-vin10t6:371_0": "119.",
  "tika-vin10t6:372_0": "Jotiṁ samādahitvāna, visibbento duve phuse;",
  "tika-vin10t6:373_0": "Payoge dukkaṭaṁ vuttaṁ, pācittiyaṁ visīvite.",
  "tika-vin10t6:374_0": "120.",
  "tika-vin10t6:375_0": "Oraso addhamāsassa, nhāyanto dve phuse have;",
  "tika-vin10t6:376_0": "Payoge dukkaṭaṁ, nhāna-ssosāne itaraṁ siyā.",
  "tika-vin10t6:377_0": "121.",
  "tika-vin10t6:378_0": "Dubbaṇṇakaraṇānaṁ tu, tiṇṇamekamanādiya;",
  "tika-vin10t6:379_0": "Cīvaraṁ paribhuñjanto, dve phuse dukkaṭādayo.",
  "tika-vin10t6:380_0": "122.",
  "tika-vin10t6:381_0": "Cīvaraṁ bhikkhuādīnaṁ, vikappetvā anuddharaṁ;",
  "tika-vin10t6:382_0": "Dve phuse paribhuñjanto, payoge dukkaṭādayo.",
  "tika-vin10t6:383_0": "123.",
  "tika-vin10t6:384_0": "Bhikkhussāpanidhento dve, phuse pattādikaṁ pana;",
  "tika-vin10t6:385_0": "Payoge dukkaṭaṁ, tasmiṁ, sesāpanihite siyā.",
  "tika-vin10t6:386_0": "Surāpānavaggo chaṭṭho.",
  "tika-vin10t6:387_0": "124.",
  "tika-vin10t6:388_0": "Sañcicca jīvitā pāṇaṁ, voropento tapodhano;",
  "tika-vin10t6:389_0": "Āpattiyo catassova, āpajjati, na saṁsayo.",
  "tika-vin10t6:390_0": "125.",
  "tika-vin10t6:391_0": "Anodissakamopātaṁ, khaṇato hoti dukkaṭaṁ;",
  "tika-vin10t6:392_0": "Manusso marati tasmiṁ, patitvā ce parājayo.",
  "tika-vin10t6:393_0": "126.",
  "tika-vin10t6:394_0": "Yakkho vāpi tiracchāna-gato manussaviggaho;",
  "tika-vin10t6:395_0": "Patitvā maratī peto, tassa thullaccayaṁ siyā.",
  "tika-vin10t6:396_0": "127.",
  "tika-vin10t6:397_0": "Tiracchānagate tasmiṁ, nipatitvā mate pana;",
  "tika-vin10t6:398_0": "Tassa pācittiyāpatti, paññattā paṭubuddhinā.",
  "tika-vin10t6:399_0": "128.",
  "tika-vin10t6:400_0": "Jānaṁ sappāṇakaṁ toyaṁ, paribhuñjaṁ duve phuse;",
  "tika-vin10t6:401_0": "Payoge dukkaṭaṁ tassa, bhutte pācittiyaṁ siyā.",
  "tika-vin10t6:402_0": "129.",
  "tika-vin10t6:403_0": "Nihatādhikaraṇaṁ jānaṁ, ukkoṭento duve phuse;",
  "tika-vin10t6:404_0": "Payoge dukkaṭaṁ vuttaṁ, pācittukkoṭite siyā.",
  "tika-vin10t6:405_0": "130.",
  "tika-vin10t6:406_0": "Jānaṁ bhikkhussa duṭṭhullaṁ, chādento pana vajjakaṁ;",
  "tika-vin10t6:407_0": "Ekamāpajjatāpattiṁ, pācittimiti dīpitaṁ.",
  "tika-vin10t6:408_0": "131.",
  "tika-vin10t6:409_0": "Ūnavīsativassaṁ tu, karonto upasampadaṁ;",
  "tika-vin10t6:410_0": "Payoge dukkaṭaṁ patto, sesā sampādite siyā.",
  "tika-vin10t6:411_0": "132.",
  "tika-vin10t6:412_0": "Jānaṁ tu theyyasatthena, saṁvidhāya saheva ca;",
  "tika-vin10t6:413_0": "Tatheva mātugāmena, maggaṁ tu paṭipajjato.",
  "tika-vin10t6:414_0": "133.",
  "tika-vin10t6:415_0": "Dve panāpattiyo honti, payoge dukkaṭaṁ mataṁ;",
  "tika-vin10t6:416_0": "Paṭipanne panuddiṭṭhaṁ, pācittiyamanantaraṁ.",
  "tika-vin10t6:417_0": "134.",
  "tika-vin10t6:418_0": "Accajaṁ pāpikaṁ diṭṭhiṁ, ñattiyā dukkaṭaṁ phuse;",
  "tika-vin10t6:419_0": "Kammavācāya osāne, hoti pācitti bhikkhuno.",
  "tika-vin10t6:420_0": "135.",
  "tika-vin10t6:421_0": "Tathākaṭānudhammena, saṁbhuñjanto duve phuse;",
  "tika-vin10t6:422_0": "Payoge dukkaṭaṁ tassa, bhutte pācittiyaṁ siyā.",
  "tika-vin10t6:423_0": "136.",
  "tika-vin10t6:424_0": "Nāsitaṁ samaṇuddesaṁpalāpento duve phuse;",
  "tika-vin10t6:425_0": "Payoge dukkaṭaṁ vuttaṁ, pācitti upalāpite.",
  "tika-vin10t6:426_0": "Sappāṇakavaggo sattamo.",
  "tika-vin10t6:427_0": "137.",
  "tika-vin10t6:428_0": "Vuccamānassa bhikkhussa, bhikkhūhi sahadhammikaṁ;",
  "tika-vin10t6:429_0": "“Na sakkhissāmi”iccevaṁ, bhaṇato dukkaṭādayo.",
  "tika-vin10t6:430_0": "138.",
  "tika-vin10t6:431_0": "Vinayaṁ tu vivaṇṇento, dve panāpattiyo phuse;",
  "tika-vin10t6:432_0": "Payoge dukkaṭaṁ tassa, pācitteva vivaṇṇite.",
  "tika-vin10t6:433_0": "139.",
  "tika-vin10t6:434_0": "Mohento dve phuse, mohe, dukkaṭaṁ tu aropite;",
  "tika-vin10t6:435_0": "Ropite pana mohasmiṁ, pācittiyamudīritaṁ.",
  "tika-vin10t6:436_0": "140.",
  "tika-vin10t6:437_0": "Pahāraṁ kupito dento, bhikkhussa dve phuse have;",
  "tika-vin10t6:438_0": "Payoge dukkaṭaṁ vuttaṁ, pācitti pahaṭe siyā.",
  "tika-vin10t6:439_0": "141.",
  "tika-vin10t6:440_0": "Bhikkhussa kupito bhikkhu, uggiraṁ talasattikaṁ;",
  "tika-vin10t6:441_0": "Dve phuse dukkaṭaṁ yoge, pācittuggirite siyā.",
  "tika-vin10t6:442_0": "142.",
  "tika-vin10t6:443_0": "Bhikkhu saṅghādisesena, amūleneva codayaṁ;",
  "tika-vin10t6:444_0": "Dve phuse dukkaṭaṁ yoge, pācittuddhaṁsite siyā.",
  "tika-vin10t6:445_0": "143.",
  "tika-vin10t6:446_0": "Bhikkhu sañcicca kukkuccaṁ, janayanto hi bhikkhuno;",
  "tika-vin10t6:447_0": "Dve phuse dukkaṭaṁ yoge, pācittuppādite siyā.",
  "tika-vin10t6:448_0": "144.",
  "tika-vin10t6:449_0": "Tiṭṭhantupassutiṁ bhikkhu, dve panāpattiyo phuse;",
  "tika-vin10t6:450_0": "Gacchato dukkaṭaṁ sotuṁ, pācitti suṇato siyā.",
  "tika-vin10t6:451_0": "145.",
  "tika-vin10t6:452_0": "Dhammikānaṁ tu kammānaṁ, chandaṁ datvā tato puna;",
  "tika-vin10t6:453_0": "Khīyanadhammamāpajjaṁ, dve phuse dukkaṭādayo.",
  "tika-vin10t6:454_0": "146.",
  "tika-vin10t6:455_0": "Saṅghe vinicchaye niṭṭhaṁ, agate chandamattano;",
  "tika-vin10t6:456_0": "Adatvā gacchato tassa, dve panāpattiyo siyuṁ.",
  "tika-vin10t6:457_0": "147.",
  "tika-vin10t6:458_0": "Hatthapāsaṁ tu saṅghassa, jahato hoti dukkaṭaṁ;",
  "tika-vin10t6:459_0": "Jahite hatthapāsasmiṁ, hoti pācitti bhikkhuno.",
  "tika-vin10t6:460_0": "148.",
  "tika-vin10t6:461_0": "Samaggena ca saṅghena, datvāna saha cīvaraṁ;",
  "tika-vin10t6:462_0": "Khīyanto dve phuse pacchā, payoge dukkaṭādayo.",
  "tika-vin10t6:463_0": "149.",
  "tika-vin10t6:464_0": "Lābhaṁ pariṇataṁ jānaṁ, saṅghikaṁ puggalassa hi;",
  "tika-vin10t6:465_0": "Dve phuse pariṇāmento, payoge dukkaṭādayo.",
  "tika-vin10t6:466_0": "Sahadhammikavaggo aṭṭhamo.",
  "tika-vin10t6:467_0": "150.",
  "tika-vin10t6:468_0": "Pubbe avidito hutvā, rañño antepuraṁ pana;",
  "tika-vin10t6:469_0": "Pavisantassa bhikkhuno, dve panāpattiyo siyuṁ.",
  "tika-vin10t6:470_0": "151.",
  "tika-vin10t6:471_0": "Paṭhamena ca pādena, ummārātikkame pana;",
  "tika-vin10t6:472_0": "Dukkaṭaṁ pana uddiṭṭhaṁ, pācitti dutiyena tu.",
  "tika-vin10t6:473_0": "152.",
  "tika-vin10t6:474_0": "Ratanaṁ pana gaṇhanto, dve panāpattiyo phuse;",
  "tika-vin10t6:475_0": "Payoge dukkaṭaṁ tassa, pācitti gahite siyā.",
  "tika-vin10t6:476_0": "153.",
  "tika-vin10t6:477_0": "Santaṁ bhikkhuṁ anāpucchā, vikāle gāmakaṁ pana;",
  "tika-vin10t6:478_0": "Samaṇo pavisaṁ dose, āpajjati duve pana.",
  "tika-vin10t6:479_0": "154.",
  "tika-vin10t6:480_0": "Paṭhamena ca pādena, parikkhepaṁ atikkame;",
  "tika-vin10t6:481_0": "Dukkaṭaṁ tassa niddiṭṭhaṁ, pācitti dutiyena tu.",
  "tika-vin10t6:482_0": "155.",
  "tika-vin10t6:483_0": "Aṭṭhidantavisāṇābhi-nibbattaṁ sūciyā gharaṁ;",
  "tika-vin10t6:484_0": "Kārāpento phuse dvepi, payoge dukkaṭādayo.",
  "tika-vin10t6:485_0": "156.",
  "tika-vin10t6:486_0": "Pamāṇātītamañcādiṁ, kārāpento duve phuse;",
  "tika-vin10t6:487_0": "Payoge dukkaṭaṁ vuttaṁ, sesā kārāpite siyā.",
  "tika-vin10t6:488_0": "157.",
  "tika-vin10t6:489_0": "Tūlonaddhaṁ tu mañcādiṁ, kārāpento duve phuse;",
  "tika-vin10t6:490_0": "Payoge dukkaṭaṁ, tasmiṁ, sesā kārāpite siyā.",
  "tika-vin10t6:491_0": "158.",
  "tika-vin10t6:492_0": "Sattame aṭṭhame ceva, navame dasamepi ca;",
  "tika-vin10t6:493_0": "Anantarasamoyeva, āpattīnaṁ vinicchayo.",
  "tika-vin10t6:494_0": "Ratanavaggo navamo.",
  "tika-vin10t6:495_0": "Pācittiyakathā.",
  "tika-vin10t6:496_0": "159.",
  "tika-vin10t6:497_0": "Catūsu duvidhāpatti, pāṭidesaniyesupi;",
  "tika-vin10t6:498_0": "Avisesena niddiṭṭhā, buddhenādiccabandhunā.",
  "tika-vin10t6:499_0": "160.",
  "tika-vin10t6:500_0": "“Bhuñjissāmī”ti bhikkhussa, dukkaṭaṁ paṭigaṇhato;",
  "tika-vin10t6:501_0": "Ajjhohāresu sabbattha, pāṭidesaniyaṁ siyā.",
  "tika-vin10t6:502_0": "Pāṭidesanīyakathā.",
  "tika-vin10t6:503_0": "161.",
  "tika-vin10t6:504_0": "Sekhiyesu ca dhammesu, ekāvāpatti dīpitā;",
  "tika-vin10t6:505_0": "Anādaravaseneva, dukkaṭaṁ samudāhaṭaṁ.",
  "tika-vin10t6:506_0": "Sekhiyakathā.",
  "tika-vin10t6:507_0": "162.",
  "tika-vin10t6:508_0": "Paññattā methunaṁ dhammaṁ, paṭisevanapaccayā;",
  "tika-vin10t6:509_0": "Kati āpattiyo honti? Catassova bhavanti hi.",
  "tika-vin10t6:510_0": "163.",
  "tika-vin10t6:511_0": "Methunaṁ paṭisevanto, allokāsappavesane;",
  "tika-vin10t6:512_0": "Mate akkhāyite vāpi, bhikkhu pārājikaṁ phuse.",
  "tika-vin10t6:513_0": "164.",
  "tika-vin10t6:514_0": "Thullaccayaṁ tu yebhuyya-kkhāyite, dukkaṭaṁ tathā;",
  "tika-vin10t6:515_0": "Mukhe vaṭṭakate vuttaṁ, pācitti jatumaṭṭhake.",
  "tika-vin10t6:516_0": "165.",
  "tika-vin10t6:517_0": "Paññattā kāyasaṁsaggaṁ, samāpajjanapaccayā;",
  "tika-vin10t6:518_0": "Kati āpattiyo honti? Pañca āpattiyo siyuṁ.",
  "tika-vin10t6:519_0": "166.",
  "tika-vin10t6:520_0": "Avassutassa posassa, tathā bhikkhuniyāpi ca;",
  "tika-vin10t6:521_0": "Pārājikamadhakkhādi-gahaṇaṁ sādiyantiyā.",
  "tika-vin10t6:522_0": "167.",
  "tika-vin10t6:523_0": "Kāyena phusato kāyaṁ, bhikkhussa garukaṁ siyā;",
  "tika-vin10t6:524_0": "Kāyena kāyabaddhaṁ tu, phusaṁ thullaccayaṁ siyā.",
  "tika-vin10t6:525_0": "168.",
  "tika-vin10t6:526_0": "Paṭibaddhena kāyena, paṭibaddhaṁ tu dukkaṭaṁ;",
  "tika-vin10t6:527_0": "Pācittiyaṁ panuddiṭṭhaṁ, tassaṅgulipatodake.",
  "tika-vin10t6:528_0": "169.",
  "tika-vin10t6:529_0": "Sesesu sekhiyantesu, āpattīnaṁ vinicchayo;",
  "tika-vin10t6:530_0": "Heṭṭhā vuttanayeneva, veditabbo vibhāvinā.",
  "tika-vin10t6:531_0": "Mahāvibhaṅgasaṅgaho niṭṭhito."
}