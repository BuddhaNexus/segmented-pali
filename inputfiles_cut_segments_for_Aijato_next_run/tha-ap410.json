{
  "tha-ap410:0.1_0": "Therāpadāna",
  "tha-ap410:0.2_0": "Metteyyavagga",
  "tha-ap410:0.3_0": "8. Todeyyattheraapadāna",
  "tha-ap410:1.1_0": "“Rājāsi vijayo nāma,",
  "tha-ap410:1.2_0": "ketumatīpuruttame;",
  "tha-ap410:1.3_0": "Sūro vikkamasampanno,",
  "tha-ap410:1.4_0": "puramajjhāvasī tadā.",
  "tha-ap410:2.1_0": "Tassa rañño pamattassa,",
  "tha-ap410:2.2_0": "aṭaviyo samuṭṭhahuṁ;",
  "tha-ap410:2.3_0": "Otārā tuṇḍikā ceva,",
  "tha-ap410:2.4_0": "raṭṭhaṁ viddhaṁsayuṁ tadā.",
  "tha-ap410:3.1_0": "Paccante kupite khippaṁ,",
  "tha-ap410:3.2_0": "sannipātesirindamo;",
  "tha-ap410:3.3_0": "Bhaṭe ceva balatthe ca,",
  "tha-ap410:3.4_0": "ariṁ niggāhayi tadā.",
  "tha-ap410:4.1_0": "Hatthārohā anīkaṭṭhā,",
  "tha-ap410:4.2_0": "sūrā ca cammayodhino;",
  "tha-ap410:4.3_0": "Dhanuggahā ca uggā ca,",
  "tha-ap410:4.4_0": "sabbe sannipatuṁ tadā.",
  "tha-ap410:5.1_0": "Āḷārikā ca kappakā,",
  "tha-ap410:5.2_0": "nhāpakā mālakārakā;",
  "tha-ap410:5.3_0": "Sūrā vijitasaṅgāmā,",
  "tha-ap410:5.4_0": "sabbe sannipatuṁ tadā.",
  "tha-ap410:6.1_0": "Khaggahatthā ca purisā,",
  "tha-ap410:6.2_0": "cāpahatthā ca vammino;",
  "tha-ap410:6.3_0": "Luddā vijitasaṅgāmā,",
  "tha-ap410:6.4_0": "sabbe sannipatuṁ tadā.",
  "tha-ap410:7.1_0": "Tidhāpabhinnā mātaṅgā,",
  "tha-ap410:7.2_0": "kuñjarā saṭṭhihāyanā;",
  "tha-ap410:7.3_0": "Suvaṇṇakacchālaṅkārā,",
  "tha-ap410:7.4_0": "sabbe sannipatuṁ tadā.",
  "tha-ap410:8.1_0": "Khamā sītassa uṇhassa,",
  "tha-ap410:8.2_0": "ukkāruharaṇassa ca;",
  "tha-ap410:8.3_0": "Yodhājīvā katakammā,",
  "tha-ap410:8.4_0": "sabbe sannipatuṁ tadā.",
  "tha-ap410:9.1_0": "Saṅkhasaddaṁ bherisaddaṁ,",
  "tha-ap410:9.2_0": "atho utujasaddakaṁ;",
  "tha-ap410:9.3_0": "Etehi te hāsayantā,",
  "tha-ap410:9.4_0": "sabbe sannipatuṁ tadā.",
  "tha-ap410:10.1_0": "Tisūlakontimantehi ca,",
  "tha-ap410:10.2_0": "kavacatomarehi ca;",
  "tha-ap410:10.3_0": "Koṭṭentānaṁ nipātentā,",
  "tha-ap410:10.4_0": "sabbe sannipatuṁ tadā.",
  "tha-ap410:11.1_0": "Kimevātinisāmetvā,",
  "tha-ap410:11.2_0": "sarājā ajitaṁ jino;",
  "tha-ap410:11.3_0": "Saṭṭhi pāṇasahassāni,",
  "tha-ap410:11.4_0": "sūle uttāsayiṁ tadā.",
  "tha-ap410:12.1_0": "Saddaṁ mānusakākaṁsu,",
  "tha-ap410:12.2_0": "‘aho rājā adhammiko;",
  "tha-ap410:12.3_0": "Niraye paccamānassa,",
  "tha-ap410:12.4_0": "kadā anto bhavissati’.",
  "tha-ap410:13.1_0": "Sayanehaṁ tuvaṭṭento,",
  "tha-ap410:13.2_0": "passāmi niraye tadā;",
  "tha-ap410:13.3_0": "Na supāmi divārattiṁ,",
  "tha-ap410:13.4_0": "sūlena tajjayanti maṁ.",
  "tha-ap410:14.1_0": "Kiṁ pamādena rajjena,",
  "tha-ap410:14.2_0": "vāhanena balena ca;",
  "tha-ap410:14.3_0": "Na te pahonti dhāretuṁ,",
  "tha-ap410:14.4_0": "tāpayanti mamaṁ sadā.",
  "tha-ap410:15.1_0": "Kiṁ me puttehi dārehi,",
  "tha-ap410:15.2_0": "rajjena sakalena ca;",
  "tha-ap410:15.3_0": "Yannūna pabbajeyyāhaṁ,",
  "tha-ap410:15.4_0": "gatimaggaṁ visodhaye.",
  "tha-ap410:16.1_0": "Saṭṭhi nāgasahassāni,",
  "tha-ap410:16.2_0": "sabbālaṅkārabhūsite;",
  "tha-ap410:16.3_0": "Suvaṇṇakacche mātaṅge,",
  "tha-ap410:16.4_0": "hemakappanavāsase.",
  "tha-ap410:17.1_0": "Ārūḷhe gāmaṇīyehi,",
  "tha-ap410:17.2_0": "tomaraṅkusapāṇibhi;",
  "tha-ap410:17.3_0": "Saṅgāmāvacare ṭhāne,",
  "tha-ap410:17.4_0": "anapekkho vihāyahaṁ;",
  "tha-ap410:17.5_0": "Sakakammena santatto,",
  "tha-ap410:17.6_0": "nikkhamiṁ anagāriyaṁ.",
  "tha-ap410:18.1_0": "Saṭṭhi assasahassāni,",
  "tha-ap410:18.2_0": "sabbālaṅkārabhūsite;",
  "tha-ap410:18.3_0": "Ājānīyeva jātiyā,",
  "tha-ap410:18.4_0": "sindhave sīghavāhane.",
  "tha-ap410:19.1_0": "Ārūḷhe gāmaṇīyehi,",
  "tha-ap410:19.2_0": "cāpahatthehi vammibhi;",
  "tha-ap410:19.3_0": "Pahāretvāna te sabbe,",
  "tha-ap410:19.4_0": "nikkhamiṁ anagāriyaṁ.",
  "tha-ap410:20.1_0": "Saṭṭhi rathasahassāni,",
  "tha-ap410:20.2_0": "sabbālaṅkārabhūsite;",
  "tha-ap410:20.3_0": "Dīpe athopi veyagghe,",
  "tha-ap410:20.4_0": "sannaddhe ussitaddhaje;",
  "tha-ap410:20.5_0": "Te sabbe parihāretvā,",
  "tha-ap410:20.6_0": "pabbajiṁ anagāriyaṁ.",
  "tha-ap410:21.1_0": "Saṭṭhi dhenusahassāni,",
  "tha-ap410:21.2_0": "sabbā kaṁsūpadhāraṇā;",
  "tha-ap410:21.3_0": "Tāyopi chaḍḍayitvāna,",
  "tha-ap410:21.4_0": "pabbajiṁ anagāriyaṁ.",
  "tha-ap410:22.1_0": "Saṭṭhi itthisahassāni,",
  "tha-ap410:22.2_0": "sabbālaṅkārabhūsitā;",
  "tha-ap410:22.3_0": "Vicittavatthābharaṇā,",
  "tha-ap410:22.4_0": "āmukkamaṇikuṇḍalā.",
  "tha-ap410:23.1_0": "Aḷārapamhā hasulā,",
  "tha-ap410:23.2_0": "susaññā tanumajjhimā;",
  "tha-ap410:23.3_0": "Tā hitvā kandamānāyo,",
  "tha-ap410:23.4_0": "pabbajiṁ anagāriyaṁ.",
  "tha-ap410:24.1_0": "Saṭṭhi gāmasahassāni,",
  "tha-ap410:24.2_0": "paripuṇṇāni sabbaso;",
  "tha-ap410:24.3_0": "Chaḍḍayitvāna taṁ rajjaṁ,",
  "tha-ap410:24.4_0": "pabbajiṁ anagāriyaṁ.",
  "tha-ap410:25.1_0": "Nagarā nikkhamitvāna,",
  "tha-ap410:25.2_0": "himavantamupāgamiṁ;",
  "tha-ap410:25.3_0": "Bhāgīrathīnadītīre,",
  "tha-ap410:25.4_0": "assamaṁ māpayiṁ ahaṁ.",
  "tha-ap410:26.1_0": "Paṇṇasālaṁ karitvāna,",
  "tha-ap410:26.2_0": "Agyāgāraṁ akāsahaṁ;",
  "tha-ap410:26.3_0": "Āraddhavīriyo pahitatto,",
  "tha-ap410:26.4_0": "Vasāmi assame ahaṁ.",
  "tha-ap410:27.1_0": "Maṇḍape rukkhamūle vā,",
  "tha-ap410:27.2_0": "suññāgāre ca jhāyato;",
  "tha-ap410:27.3_0": "Na tu vijjati tāso me,",
  "tha-ap410:27.4_0": "na passe bhayabheravaṁ.",
  "tha-ap410:28.1_0": "Sumedho nāma sambuddho,",
  "tha-ap410:28.2_0": "aggo kāruṇiko muni;",
  "tha-ap410:28.3_0": "Ñāṇālokena jotanto,",
  "tha-ap410:28.4_0": "loke uppajji tāvade.",
  "tha-ap410:29.1_0": "Mama assamasāmantā,",
  "tha-ap410:29.2_0": "yakkho āsi mahiddhiko;",
  "tha-ap410:29.3_0": "Buddhaseṭṭhamhi uppanne,",
  "tha-ap410:29.4_0": "ārocesi mamaṁ tadā.",
  "tha-ap410:30.1_0": "‘Buddho loke samuppanno,",
  "tha-ap410:30.2_0": "sumedho nāma cakkhumā;",
  "tha-ap410:30.3_0": "Tāreti janataṁ sabbaṁ,",
  "tha-ap410:30.4_0": "tampi so tārayissati’.",
  "tha-ap410:31.1_0": "Yakkhassa vacanaṁ sutvā,",
  "tha-ap410:31.2_0": "saṁviggo āsi tāvade;",
  "tha-ap410:31.3_0": "Buddho buddhoti cintento,",
  "tha-ap410:31.4_0": "assamaṁ paṭisāmayiṁ.",
  "tha-ap410:32.1_0": "Aggidāruñca chaḍḍetvā,",
  "tha-ap410:32.2_0": "saṁsāmetvāna santhataṁ;",
  "tha-ap410:32.3_0": "Assamaṁ abhivanditvā,",
  "tha-ap410:32.4_0": "nikkhamiṁ vipinā ahaṁ.",
  "tha-ap410:33.1_0": "Tato candanamādāya,",
  "tha-ap410:33.2_0": "gāmā gāmaṁ purā puraṁ;",
  "tha-ap410:33.3_0": "Devadevaṁ gavesanto,",
  "tha-ap410:33.4_0": "upagacchiṁ vināyakaṁ.",
  "tha-ap410:34.1_0": "Bhagavā tamhi samaye,",
  "tha-ap410:34.2_0": "sumedho lokanāyako;",
  "tha-ap410:34.3_0": "Catusaccaṁ pakāsento,",
  "tha-ap410:34.4_0": "bodheti janataṁ bahuṁ.",
  "tha-ap410:35.1_0": "Añjaliṁ paggahetvāna,",
  "tha-ap410:35.2_0": "sīse katvāna candanaṁ;",
  "tha-ap410:35.3_0": "Sambuddhaṁ abhivādetvā,",
  "tha-ap410:35.4_0": "imā gāthā abhāsahaṁ.",
  "tha-ap410:36.1_0": "‘Vassike pupphamānamhi,",
  "tha-ap410:36.2_0": "santike upavāyati;",
  "tha-ap410:36.3_0": "Tvaṁ vīra guṇagandhena,",
  "tha-ap410:36.4_0": "disā sabbā pavāyasi.",
  "tha-ap410:37.1_0": "Campake nāgavanike,",
  "tha-ap410:37.2_0": "atimuttakaketake;",
  "tha-ap410:37.3_0": "Sālesu pupphamānesu,",
  "tha-ap410:37.4_0": "anuvātaṁ pavāyati.",
  "tha-ap410:38.1_0": "Tava gandhaṁ suṇitvāna,",
  "tha-ap410:38.2_0": "himavantā idhāgamiṁ;",
  "tha-ap410:38.3_0": "Pūjemi taṁ mahāvīra,",
  "tha-ap410:38.4_0": "lokajeṭṭha mahāyasa’.",
  "tha-ap410:39.1_0": "Varacandanenānulimpiṁ,",
  "tha-ap410:39.2_0": "sumedhaṁ lokanāyakaṁ;",
  "tha-ap410:39.3_0": "Sakaṁ cittaṁ pasādetvā,",
  "tha-ap410:39.4_0": "tuṇhī aṭṭhāsi tāvade.",
  "tha-ap410:40.1_0": "Sumedho nāma bhagavā,",
  "tha-ap410:40.2_0": "lokajeṭṭho narāsabho;",
  "tha-ap410:40.3_0": "Bhikkhusaṅghe nisīditvā,",
  "tha-ap410:40.4_0": "imā gāthā abhāsatha.",
  "tha-ap410:41.1_0": "‘Yo me guṇe pakittesi,",
  "tha-ap410:41.2_0": "candanañca apūjayi;",
  "tha-ap410:41.3_0": "Tamahaṁ kittayissāmi,",
  "tha-ap410:41.4_0": "suṇātha mama bhāsato.",
  "tha-ap410:42.1_0": "Ādeyyavākyavacano,",
  "tha-ap410:42.2_0": "brahmā uju patāpavā;",
  "tha-ap410:42.3_0": "Pañcavīsatikappāni,",
  "tha-ap410:42.4_0": "sappabhāso bhavissati.",
  "tha-ap410:43.1_0": "Chabbīsatikappasate,",
  "tha-ap410:43.2_0": "devaloke ramissati;",
  "tha-ap410:43.3_0": "Sahassakkhattuṁ rājā ca,",
  "tha-ap410:43.4_0": "cakkavattī bhavissati.",
  "tha-ap410:44.1_0": "Tettiṁsakkhattuṁ devindo,",
  "tha-ap410:44.2_0": "devarajjaṁ karissati;",
  "tha-ap410:44.3_0": "Padesarajjaṁ vipulaṁ,",
  "tha-ap410:44.4_0": "gaṇanāto asaṅkhiyaṁ.",
  "tha-ap410:45.1_0": "Tato cutoyaṁ manujo,",
  "tha-ap410:45.2_0": "manussattaṁ gamissati;",
  "tha-ap410:45.3_0": "Puññakammena saṁyutto,",
  "tha-ap410:45.4_0": "brahmabandhu bhavissati.",
  "tha-ap410:46.1_0": "Ajjhāyako mantadharo,",
  "tha-ap410:46.2_0": "tiṇṇaṁ vedāna pāragū;",
  "tha-ap410:46.3_0": "Tilakkhaṇena sampanno,",
  "tha-ap410:46.4_0": "bāvarī nāma brāhmaṇo.",
  "tha-ap410:47.1_0": "Tassa sisso bhavitvāna,",
  "tha-ap410:47.2_0": "hessati mantapāragū;",
  "tha-ap410:47.3_0": "Upagantvāna sambuddhaṁ,",
  "tha-ap410:47.4_0": "gotamaṁ sakyapuṅgavaṁ.",
  "tha-ap410:48.1_0": "Pucchitvā nipuṇe pañhe,",
  "tha-ap410:48.2_0": "bhāvayitvāna añjasaṁ;",
  "tha-ap410:48.3_0": "Sabbāsave pariññāya,",
  "tha-ap410:48.4_0": "viharissatināsavo’.",
  "tha-ap410:49.1_0": "Tividhaggi nibbutā mayhaṁ,",
  "tha-ap410:49.2_0": "bhavā sabbe samūhatā;",
  "tha-ap410:49.3_0": "Sabbāsave pariññāya,",
  "tha-ap410:49.4_0": "viharāmi anāsavo.",
  "tha-ap410:50.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap410:50.2_0": "…pe…",
  "tha-ap410:50.3_0": "viharāmi anāsavo.",
  "tha-ap410:51.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap410:51.2_0": "…pe…",
  "tha-ap410:51.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap410:52.1_0": "Paṭisambhidā catasso,",
  "tha-ap410:52.2_0": "…pe…",
  "tha-ap410:52.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap410:53.1_0": "Itthaṁ sudaṁ āyasmā todeyyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap410:54.1_0": "Todeyyattherassāpadānaṁ aṭṭhamaṁ."
}