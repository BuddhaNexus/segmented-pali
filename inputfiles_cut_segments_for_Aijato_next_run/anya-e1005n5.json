{
  "anya-e1005n5:0_0": "6. Rājakaṇḍo",
  "anya-e1005n5:1_0": "112.",
  "anya-e1005n5:2_0": "Ekayāmaṁ saye rājā,",
  "anya-e1005n5:3_0": "Dviyāmaññeva paṇḍito;",
  "anya-e1005n5:4_0": "Gharāvāso tiyāmaṁva,",
  "anya-e1005n5:5_0": "Catuyāmaṁ tu yācako.",
  "anya-e1005n5:6_0": "113.",
  "anya-e1005n5:7_0": "Dhanavā sutavā rājā,",
  "anya-e1005n5:8_0": "Nadī vejjo cimepañca;",
  "anya-e1005n5:9_0": "Yattha dese na vijjanti,",
  "anya-e1005n5:10_0": "Na tattha divasaṁ vase.",
  "anya-e1005n5:11_0": "114.",
  "anya-e1005n5:12_0": "Yasmiṁ padese na māno,",
  "anya-e1005n5:13_0": "Na pemaṁ naca bandhavā;",
  "anya-e1005n5:14_0": "Naca vijjāgamo koci,",
  "anya-e1005n5:15_0": "Na tattha divasaṁ vase.",
  "anya-e1005n5:16_0": "115.",
  "anya-e1005n5:17_0": "Aputtakaṁ gharaṁ suññaṁ,",
  "anya-e1005n5:18_0": "Raṭṭhaṁ suññaṁ arājakaṁ;",
  "anya-e1005n5:19_0": "A sippassa mukhaṁ suññaṁ,",
  "anya-e1005n5:20_0": "Sabba suññaṁ daliddakā.",
  "anya-e1005n5:21_0": "116.",
  "anya-e1005n5:22_0": "Dhanamiccheyya vāṇijjo,",
  "anya-e1005n5:23_0": "Vijjamicche bhajesutaṁ;",
  "anya-e1005n5:24_0": "Puttamicche taruṇitthiṁ,",
  "anya-e1005n5:25_0": "Rājāmaccaṁ vasaṁ game.",
  "anya-e1005n5:26_0": "117.",
  "anya-e1005n5:27_0": "Naṭṭhoyati asantuṭṭho,",
  "anya-e1005n5:28_0": "Santuṭṭho ca mahīpati;",
  "anya-e1005n5:29_0": "Lajjā ca gaṇikā naṭṭhā,",
  "anya-e1005n5:30_0": "Nillajjā kuladhītikā.",
  "anya-e1005n5:31_0": "118.",
  "anya-e1005n5:32_0": "Pakkhīnaṁ balamākāso,",
  "anya-e1005n5:33_0": "Macchānamudakaṁ balaṁ;",
  "anya-e1005n5:34_0": "Dubbalassa balaṁ rājā,",
  "anya-e1005n5:35_0": "Kumārānaṁ rudaṁ balaṁ.",
  "anya-e1005n5:36_0": "119.",
  "anya-e1005n5:37_0": "Khamā jāgariyuṭṭhānaṁ,",
  "anya-e1005n5:38_0": "Saṁvibhāgo dayikkhaṇā;",
  "anya-e1005n5:39_0": "Nāyakassa guṇā ete,",
  "anya-e1005n5:40_0": "Icchitabbā sataṁ guṇā.",
  "anya-e1005n5:41_0": "120.",
  "anya-e1005n5:42_0": "Sakiṁ vadanti rājāno,",
  "anya-e1005n5:43_0": "Sakiṁ samaṇabrāhmaṇā;",
  "anya-e1005n5:44_0": "Sakiṁ sappurisā loke,",
  "anya-e1005n5:45_0": "Esa dhammo sanantano.",
  "anya-e1005n5:46_0": "121.",
  "anya-e1005n5:47_0": "Alaso gihī kāmabhogī na sādhu,",
  "anya-e1005n5:48_0": "Asaññato pabbajito na sādhu;",
  "anya-e1005n5:49_0": "Rājā anisammakārī na sādhu,",
  "anya-e1005n5:50_0": "Paṇḍito kodhano taṁpi na sādhu.",
  "anya-e1005n5:51_0": "122.",
  "anya-e1005n5:52_0": "Bahavo yattha nettāro,",
  "anya-e1005n5:53_0": "Sabbe paṇḍitamānino;",
  "anya-e1005n5:54_0": "Sabbe mahattamicchanti,",
  "anya-e1005n5:55_0": "Tesaṁ kammaṁ vinassati.",
  "anya-e1005n5:56_0": "123.",
  "anya-e1005n5:57_0": "Āyaṁ khayaṁ sayaṁ jaññā,",
  "anya-e1005n5:58_0": "Rājā sayaṁ katākataṁ;",
  "anya-e1005n5:59_0": "Niggahe niggahetabbaṁ,",
  "anya-e1005n5:60_0": "Paggahe paggahārahaṁ.",
  "anya-e1005n5:61_0": "124.",
  "anya-e1005n5:62_0": "Piṭṭhitokkaṁ niseveyya,",
  "anya-e1005n5:63_0": "Kucchināva hutāsanaṁ;",
  "anya-e1005n5:64_0": "Sāmikaṁ sabbabhāgena,",
  "anya-e1005n5:65_0": "Paralokaṁ amohavā.",
  "anya-e1005n5:66_0": "125.",
  "anya-e1005n5:67_0": "Aggi āpo itthimūḷho,",
  "anya-e1005n5:68_0": "Sappo rāja-kulānica;",
  "anya-e1005n5:69_0": "Apayantena gantabbā,",
  "anya-e1005n5:70_0": "Acceka-pāṇahārakā.",
  "anya-e1005n5:71_0": "126.",
  "anya-e1005n5:72_0": "Paduṭṭha-bhariya-saṁvāso,",
  "anya-e1005n5:73_0": "Paduṭṭha citta dāsako;",
  "anya-e1005n5:74_0": "Sa-sappe ca ghare vāso,",
  "anya-e1005n5:75_0": "Maccu eva na saṁsayo.",
  "anya-e1005n5:76_0": "127.",
  "anya-e1005n5:77_0": "Mūḷha sisso padesena,",
  "anya-e1005n5:78_0": "Kunārī bharaṇena ca;",
  "anya-e1005n5:79_0": "Asatā sampayogena,",
  "anya-e1005n5:80_0": "Paṇḍitoppavasīdati.",
  "anya-e1005n5:81_0": "128.",
  "anya-e1005n5:82_0": "Mātā puttakaraṁ pāpaṁ,",
  "anya-e1005n5:83_0": "Sissapāpaṁ gurukatā;",
  "anya-e1005n5:84_0": "Rājā raṭṭhakaraṁ pāpaṁ,",
  "anya-e1005n5:85_0": "Rājapāpaṁ purohito.",
  "anya-e1005n5:86_0": "129.",
  "anya-e1005n5:87_0": "Akodhena jine kodhaṁ,",
  "anya-e1005n5:88_0": "Asādhuṁ sādhunā jine;",
  "anya-e1005n5:89_0": "Jine macchariṁ dānena,",
  "anya-e1005n5:90_0": "Saccenālīkavādinaṁ;",
  "anya-e1005n5:91_0": "130.",
  "anya-e1005n5:92_0": "Adantaṁ damanaṁ dānaṁ,",
  "anya-e1005n5:93_0": "Dānaṁ sabbattha sādhakaṁ;",
  "anya-e1005n5:94_0": "Dānena piya vācāya,",
  "anya-e1005n5:95_0": "Unnamanti namanti ca;",
  "anya-e1005n5:96_0": "131.",
  "anya-e1005n5:97_0": "Dānaṁ sinehabhesajjaṁ,",
  "anya-e1005n5:98_0": "Maccheraṁ dosanosadhaṁ;",
  "anya-e1005n5:99_0": "Dānaṁ yasassī bhesajjaṁ,",
  "anya-e1005n5:100_0": "Maccheraṁ kapaṇosadhaṁ.",
  "anya-e1005n5:101_0": "132.",
  "anya-e1005n5:102_0": "Bahūnamappasārānaṁ,",
  "anya-e1005n5:103_0": "Sāmaggiyā jayaṁ jaye;",
  "anya-e1005n5:104_0": "Tiṇehi vattate yottaṁ,",
  "anya-e1005n5:105_0": "Tena nāgopi bajjhate.",
  "anya-e1005n5:106_0": "133.",
  "anya-e1005n5:107_0": "Sahāyo asamatthopi,",
  "anya-e1005n5:108_0": "Tejasā kiṁkarissati;",
  "anya-e1005n5:109_0": "Nivāte jalito aggi,",
  "anya-e1005n5:110_0": "Sayame vūpasampati.",
  "anya-e1005n5:111_0": "134.",
  "anya-e1005n5:112_0": "Na raññā samakaṁ bhuñje,",
  "anya-e1005n5:113_0": "Kāmabhogaṁ kudācanaṁ;",
  "anya-e1005n5:114_0": "Ākappaṁ rasa bhuttiṁvā,",
  "anya-e1005n5:115_0": "Mālā gandha vilepanaṁ;",
  "anya-e1005n5:116_0": "Vatthaṁ sabbaalaṅkāraṁ,",
  "anya-e1005n5:117_0": "Na raññā sadisaṁ kare.",
  "anya-e1005n5:118_0": "135.",
  "anya-e1005n5:119_0": "Na me rājā sakhā hoti,",
  "anya-e1005n5:120_0": "Na rājā hoti methuno;",
  "anya-e1005n5:121_0": "Eso sāmiko mayhanti,",
  "anya-e1005n5:122_0": "Citte niṭṭhaṁ suthāpaye.",
  "anya-e1005n5:123_0": "136.",
  "anya-e1005n5:124_0": "Nātidūre bhaje rañño,",
  "anya-e1005n5:125_0": "Nāccāsannopavātake;",
  "anya-e1005n5:126_0": "Ujuke nātininne ca,",
  "anya-e1005n5:127_0": "Na bhaje uccamāsane.",
  "anya-e1005n5:128_0": "Chadose vajje sevako,",
  "anya-e1005n5:129_0": "Tiṭṭhe aggiṁva saṁyato.",
  "anya-e1005n5:130_0": "137.",
  "anya-e1005n5:131_0": "Guṇī sabbaññu tulyopi,",
  "anya-e1005n5:132_0": "Nasobhati anissayo;",
  "anya-e1005n5:133_0": "Anagghamopi maṇiseṭṭho,",
  "anya-e1005n5:134_0": "Hemaṁ nissāya sobhati.",
  "anya-e1005n5:135_0": "Rājakaṇḍo niṭṭhito."
}