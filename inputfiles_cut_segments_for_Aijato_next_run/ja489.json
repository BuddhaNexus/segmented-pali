{
  "ja489:0.1_0": "Jātaka",
  "ja489:0.2_0": "Pakiṇṇakanipāta",
  "ja489:0.3_0": "Sālikedāravagga",
  "ja489:0.4_0": "6. Surucijātaka",
  "ja489:1.1_0": "“Mahesī surucino bhariyā,",
  "ja489:1.2_0": "Ānītā paṭhamaṁ ahaṁ;",
  "ja489:1.3_0": "Dasa vassasahassāni,",
  "ja489:1.4_0": "Yaṁ maṁ surucimānayi.",
  "ja489:2.1_0": "Sāhaṁ brāhmaṇa rājānaṁ,",
  "ja489:2.2_0": "vedehaṁ mithilaggahaṁ;",
  "ja489:2.3_0": "Nābhijānāmi kāyena,",
  "ja489:2.4_0": "vācāya uda cetasā;",
  "ja489:2.5_0": "Suruciṁ atimaññittha,",
  "ja489:2.6_0": "āvi vā yadi vā raho.",
  "ja489:3.1_0": "Etena saccavajjena,",
  "ja489:3.2_0": "putto uppajjataṁ ise;",
  "ja489:3.3_0": "Musā me bhaṇamānāya,",
  "ja489:3.4_0": "muddhā phalatu sattadhā.",
  "ja489:4.1_0": "Bhattu mama sassu mātā,",
  "ja489:4.2_0": "pitā cāpi ca sassuro;",
  "ja489:4.3_0": "Te maṁ brahme vinetāro,",
  "ja489:4.4_0": "yāva aṭṭhaṁsu jīvitaṁ.",
  "ja489:5.1_0": "Sāhaṁ ahiṁsāratinī,",
  "ja489:5.2_0": "kāmasā dhammacārinī;",
  "ja489:5.3_0": "Sakkaccaṁ te upaṭṭhāsiṁ,",
  "ja489:5.4_0": "rattindivamatanditā.",
  "ja489:6.1_0": "Etena saccavajjena,",
  "ja489:6.2_0": "putto uppajjataṁ ise;",
  "ja489:6.3_0": "Musā me bhaṇamānāya,",
  "ja489:6.4_0": "muddhā phalatu sattadhā.",
  "ja489:7.1_0": "Soḷasitthisahassāni,",
  "ja489:7.2_0": "sahabhariyāni brāhmaṇa;",
  "ja489:7.3_0": "Tāsu issā vā kodho vā,",
  "ja489:7.4_0": "nāhu mayhaṁ kudācanaṁ.",
  "ja489:8.1_0": "Hitena tāsaṁ nandāmi,",
  "ja489:8.2_0": "na ca me kāci appiyā;",
  "ja489:8.3_0": "Attānaṁvānukampāmi,",
  "ja489:8.4_0": "sadā sabbā sapattiyo.",
  "ja489:9.1_0": "Etena saccavajjena,",
  "ja489:9.2_0": "putto uppajjataṁ ise;",
  "ja489:9.3_0": "Musā me bhaṇamānāya,",
  "ja489:9.4_0": "muddhā phalatu sattadhā.",
  "ja489:10.1_0": "Dāse kammakare pesse,",
  "ja489:10.2_0": "ye caññe anujīvino;",
  "ja489:10.3_0": "Pesemi sahadhammena,",
  "ja489:10.4_0": "sadā pamuditindriyā.",
  "ja489:11.1_0": "Etena saccavajjena,",
  "ja489:11.2_0": "putto uppajjataṁ ise;",
  "ja489:11.3_0": "Musā me bhaṇamānāya,",
  "ja489:11.4_0": "muddhā phalatu sattadhā.",
  "ja489:12.1_0": "Samaṇe brāhmaṇe cāpi,",
  "ja489:12.2_0": "aññe cāpi vanibbake;",
  "ja489:12.3_0": "Tappemi annapānena,",
  "ja489:12.4_0": "sadā payatapāṇinī.",
  "ja489:13.1_0": "Etena saccavajjena,",
  "ja489:13.2_0": "putto uppajjataṁ ise;",
  "ja489:13.3_0": "Musā me bhaṇamānāya,",
  "ja489:13.4_0": "muddhā phalatu sattadhā.",
  "ja489:14.1_0": "Cātuddasiṁ pañcaddasiṁ,",
  "ja489:14.2_0": "Yā ca pakkhassa aṭṭhamī;",
  "ja489:14.3_0": "Pāṭihāriyapakkhañca,",
  "ja489:14.4_0": "Aṭṭhaṅgasusamāgataṁ;",
  "ja489:14.5_0": "Uposathaṁ upavasāmi,",
  "ja489:14.6_0": "Sadā sīlesu saṁvutā.",
  "ja489:15.1_0": "Etena saccavajjena,",
  "ja489:15.2_0": "putto uppajjataṁ ise;",
  "ja489:15.3_0": "Musā me bhaṇamānāya,",
  "ja489:15.4_0": "muddhā phalatu sattadhā”.",
  "ja489:16.1_0": "“Sabbeva te dhammaguṇā,",
  "ja489:16.2_0": "rājaputti yasassini;",
  "ja489:16.3_0": "Saṁvijjanti tayi bhadde,",
  "ja489:16.4_0": "ye tvaṁ kittesi attani.",
  "ja489:17.1_0": "Khattiyo jātisampanno,",
  "ja489:17.2_0": "abhijāto yasassimā;",
  "ja489:17.3_0": "Dhammarājā videhānaṁ,",
  "ja489:17.4_0": "putto uppajjate tava”.",
  "ja489:18.1_0": "“Dummī rajojalladharo,",
  "ja489:18.2_0": "aghe vehāyasaṁ ṭhito;",
  "ja489:18.3_0": "Manuññaṁ bhāsase vācaṁ,",
  "ja489:18.4_0": "yaṁ mayhaṁ hadayaṅgamaṁ.",
  "ja489:19.1_0": "Devatānusi saggamhā,",
  "ja489:19.2_0": "isi vāsi mahiddhiko;",
  "ja489:19.3_0": "Ko vāsi tvaṁ anuppatto,",
  "ja489:19.4_0": "attānaṁ me pavedaya”.",
  "ja489:20.1_0": "“Yaṁ devasaṅghā vandanti,",
  "ja489:20.2_0": "sudhammāyaṁ samāgatā;",
  "ja489:20.3_0": "Sohaṁ sakko sahassakkho,",
  "ja489:20.4_0": "āgatosmi tavantike.",
  "ja489:21.1_0": "Itthiyo jīvalokasmiṁ,",
  "ja489:21.2_0": "yā hoti samacārinī;",
  "ja489:21.3_0": "Medhāvinī sīlavatī,",
  "ja489:21.4_0": "sassudevā patibbatā.",
  "ja489:22.1_0": "Tādisāya sumedhāya,",
  "ja489:22.2_0": "sucikammāya nāriyā;",
  "ja489:22.3_0": "Devā dassanamāyanti,",
  "ja489:22.4_0": "mānusiyā amānusā.",
  "ja489:23.1_0": "Tvañca bhadde suciṇṇena,",
  "ja489:23.2_0": "pubbe sucaritena ca;",
  "ja489:23.3_0": "Idha rājakule jātā,",
  "ja489:23.4_0": "sabbakāmasamiddhinī.",
  "ja489:24.1_0": "Ayañca te rājaputti,",
  "ja489:24.2_0": "ubhayattha kaṭaggaho;",
  "ja489:24.3_0": "Devalokūpapattī ca,",
  "ja489:24.4_0": "kittī ca idha jīvite.",
  "ja489:25.1_0": "Ciraṁ sumedhe sukhinī,",
  "ja489:25.2_0": "dhammamattani pālaya;",
  "ja489:25.3_0": "Esāhaṁ tidivaṁ yāmi,",
  "ja489:25.4_0": "piyaṁ me tava dassanan”ti.",
  "ja489:26.1_0": "Surucijātakaṁ chaṭṭhaṁ."
}