{
  "tha-ap383:0.1_0": "Therāpadāna",
  "tha-ap383:0.2_0": "Avaṭaphalavagga",
  "tha-ap383:0.3_0": "1 Avaṭaphaladāyakattheraapadāna",
  "tha-ap383:1.1_0": "“Sataraṁsi nāma bhagavā,",
  "tha-ap383:1.2_0": "sayambhū aparājito;",
  "tha-ap383:1.3_0": "Vivekakāmo sambuddho,",
  "tha-ap383:1.4_0": "gocarāyābhinikkhami.",
  "tha-ap383:2.1_0": "Phalahattho ahaṁ disvā,",
  "tha-ap383:2.2_0": "upagacchiṁ narāsabhaṁ;",
  "tha-ap383:2.3_0": "Pasannacitto sumano,",
  "tha-ap383:2.4_0": "avaṭaṁ adadiṁ phalaṁ.",
  "tha-ap383:3.1_0": "Catunnavutito kappe,",
  "tha-ap383:3.2_0": "yaṁ phalamadadiṁ ahaṁ;",
  "tha-ap383:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap383:3.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap383:4.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap383:4.2_0": "bhavā sabbe samūhatā;",
  "tha-ap383:4.3_0": "Nāgova bandhanaṁ chetvā,",
  "tha-ap383:4.4_0": "viharāmi anāsavo.",
  "tha-ap383:5.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap383:5.2_0": "mama buddhassa santike;",
  "tha-ap383:5.3_0": "Tisso vijjā anuppattā,",
  "tha-ap383:5.4_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap383:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap383:6.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap383:6.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap383:6.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap383:7.1_0": "Itthaṁ sudaṁ āyasmā avaṭaphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap383:8.1_0": "Avaṭaphaladāyakattherassāpadānaṁ paṭhamaṁ."
}