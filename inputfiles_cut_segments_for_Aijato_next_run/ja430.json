{
  "ja430:0.1_0": "Jātaka",
  "ja430:0.2_0": "Navakanipāta",
  "ja430:0.3_0": "Gijjhavagga",
  "ja430:0.4_0": "4. Cūḷasuvajātaka",
  "ja430:1.1_0": "“Santi rukkhā haripattā,",
  "ja430:1.2_0": "dumā nekaphalā bahū;",
  "ja430:1.3_0": "Kasmā nu sukkhe koḷāpe,",
  "ja430:1.4_0": "suvassa nirato mano”.",
  "ja430:2.1_0": "“Phalassa upabhuñjimhā,",
  "ja430:2.2_0": "nekavassagaṇe bahū;",
  "ja430:2.3_0": "Aphalampi viditvāna,",
  "ja430:2.4_0": "sāva metti yathā pure”.",
  "ja430:3.1_0": "“Sukkhañca rukkhaṁ koḷāpaṁ,",
  "ja430:3.2_0": "opattamaphalaṁ dumaṁ;",
  "ja430:3.3_0": "Ohāya sakuṇā yanti,",
  "ja430:3.4_0": "kiṁ dosaṁ passase dija”.",
  "ja430:4.1_0": "“Ye phalatthā sambhajanti,",
  "ja430:4.2_0": "aphaloti jahanti naṁ;",
  "ja430:4.3_0": "Attatthapaññā dummedhā,",
  "ja430:4.4_0": "te honti pakkhapātino”.",
  "ja430:5.1_0": "“Sādhu sakkhi kataṁ hoti,",
  "ja430:5.2_0": "metti saṁsati santhavo;",
  "ja430:5.3_0": "Sacetaṁ dhammaṁ rocesi,",
  "ja430:5.4_0": "pāsaṁsosi vijānataṁ.",
  "ja430:6.1_0": "So te suva varaṁ dammi,",
  "ja430:6.2_0": "pattayāna vihaṅgama;",
  "ja430:6.3_0": "Varaṁ varassu vakkaṅga,",
  "ja430:6.4_0": "yaṁ kiñci manasicchasi”.",
  "ja430:7.1_0": "“Api nāma naṁ passeyyaṁ,",
  "ja430:7.2_0": "sapattaṁ saphalaṁ dumaṁ;",
  "ja430:7.3_0": "Daliddova nidhiṁ laddhā,",
  "ja430:7.4_0": "nandeyyāhaṁ punappunaṁ”.",
  "ja430:8.1_0": "“Tato amatamādāya,",
  "ja430:8.2_0": "abhisiñci mahīruhaṁ;",
  "ja430:8.3_0": "Tassa sākhā virūhiṁsu,",
  "ja430:8.4_0": "sītacchāyā manoramā”.",
  "ja430:9.1_0": "“Evaṁ sakka sukhī hohi,",
  "ja430:9.2_0": "saha sabbehi ñātibhi;",
  "ja430:9.3_0": "Yathāhamajja sukhito,",
  "ja430:9.4_0": "disvāna saphalaṁ dumaṁ.",
  "ja430:10.1_0": "Suvassa ca varaṁ datvā,",
  "ja430:10.2_0": "katvāna saphalaṁ dumaṁ;",
  "ja430:10.3_0": "Pakkāmi saha bhariyāya,",
  "ja430:10.4_0": "devānaṁ nandanaṁ vanan”ti.",
  "ja430:11.1_0": "Cūḷasuvajātakaṁ catutthaṁ."
}