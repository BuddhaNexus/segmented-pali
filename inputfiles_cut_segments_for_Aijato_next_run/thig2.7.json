{
  "thig2.7:0.1_0": "Therīgāthā",
  "thig2.7:0.2_0": "Dukanipāta",
  "thig2.7:0.3_0": "7. Mittātherīgāthā",
  "thig2.7:1.1_0": "“Cātuddasiṁ pañcadasiṁ,",
  "thig2.7:1.2_0": "yā ca pakkhassa aṭṭhamī;",
  "thig2.7:1.3_0": "Pāṭihāriyapakkhañca,",
  "thig2.7:1.4_0": "aṭṭhaṅgasusamāgataṁ.",
  "thig2.7:2.1_0": "Uposathaṁ upāgacchiṁ,",
  "thig2.7:2.2_0": "devakāyābhinandinī;",
  "thig2.7:2.3_0": "Sājja ekena bhattena,",
  "thig2.7:2.4_0": "muṇḍā saṅghāṭipārutā;",
  "thig2.7:2.5_0": "Devakāyaṁ na patthehaṁ,",
  "thig2.7:2.6_0": "vineyya hadaye daran”ti.",
  "thig2.7:3.1_0": "… Mittā therī …."
}