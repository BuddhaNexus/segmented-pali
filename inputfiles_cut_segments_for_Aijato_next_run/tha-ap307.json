{
  "tha-ap307:0.1_0": "Therāpadāna",
  "tha-ap307:0.2_0": "Padumakesaravagga",
  "tha-ap307:0.3_0": "5. Phaladāyakattheraapadāna",
  "tha-ap307:1.1_0": "“Bhāgīrathīnadītīre,",
  "tha-ap307:1.2_0": "ahosi assamo tadā;",
  "tha-ap307:1.3_0": "Tamahaṁ assamaṁ gacchiṁ,",
  "tha-ap307:1.4_0": "phalahattho apekkhavā.",
  "tha-ap307:2.1_0": "Vipassiṁ tattha addakkhiṁ,",
  "tha-ap307:2.2_0": "pītaraṁsiṁva bhāṇumaṁ;",
  "tha-ap307:2.3_0": "Yaṁ me atthi phalaṁ sabbaṁ,",
  "tha-ap307:2.4_0": "adāsiṁ satthuno ahaṁ.",
  "tha-ap307:3.1_0": "Ekanavutito kappe,",
  "tha-ap307:3.2_0": "yaṁ phalaṁ adadiṁ tadā;",
  "tha-ap307:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap307:3.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap307:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap307:4.2_0": "…pe…",
  "tha-ap307:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap307:5.1_0": "Itthaṁ sudaṁ āyasmā phaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap307:6.1_0": "Phaladāyakattherassāpadānaṁ pañcamaṁ."
}