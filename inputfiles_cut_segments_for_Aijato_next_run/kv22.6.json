{
  "kv22.6:0.1_0": "Kathāvatthu",
  "kv22.6:0.2_0": "Mahāpaṇṇāsaka",
  "kv22.6:0.3_0": "Bāvīsatimavagga",
  "kv22.6:0.4_0": "Abyākatakathā",
  "kv22.6:1.1_0": "Sabbaṁ supinagatassa cittaṁ abyākatanti?",
  "kv22.6:1.2_0": "Āmantā.",
  "kv22.6:1.3_0": "Supinantena pāṇaṁ haneyyāti?",
  "kv22.6:1.4_0": "Āmantā.",
  "kv22.6:1.5_0": "Hañci supinantena pāṇaṁ haneyya, no ca vata re vattabbe—",
  "kv22.6:1.6_0": "“sabbaṁ supinagatassa cittaṁ abyākatan”ti.",
  "kv22.6:2.1_0": "Supinantena adinnaṁ ādiyeyya …pe…",
  "kv22.6:2.2_0": "musā bhaṇeyya,",
  "kv22.6:2.3_0": "pisuṇaṁ bhaṇeyya,",
  "kv22.6:2.4_0": "pharusaṁ bhaṇeyya,",
  "kv22.6:2.5_0": "samphaṁ palapeyya,",
  "kv22.6:2.6_0": "sandhiṁ chindeyya,",
  "kv22.6:2.7_0": "nillopaṁ hareyya,",
  "kv22.6:2.8_0": "ekāgārikaṁ kareyya,",
  "kv22.6:2.9_0": "paripanthe tiṭṭheyya,",
  "kv22.6:2.10_0": "paradāraṁ gaccheyya,",
  "kv22.6:2.11_0": "gāmaghātakaṁ kareyya,",
  "kv22.6:2.12_0": "nigamaghātakaṁ kareyya,",
  "kv22.6:2.13_0": "supinantena methunaṁ dhammaṁ paṭiseveyya,",
  "kv22.6:2.14_0": "supinagatassa asuci mucceyya,",
  "kv22.6:2.15_0": "supinantena dānaṁ dadeyya,",
  "kv22.6:2.16_0": "cīvaraṁ dadeyya,",
  "kv22.6:2.17_0": "piṇḍapātaṁ dadeyya,",
  "kv22.6:2.18_0": "senāsanaṁ dadeyya,",
  "kv22.6:2.19_0": "gilānapaccayabhesajjaparikkhāraṁ dadeyya,",
  "kv22.6:2.20_0": "khādanīyaṁ dadeyya,",
  "kv22.6:2.21_0": "bhojanīyaṁ dadeyya,",
  "kv22.6:2.22_0": "pānīyaṁ dadeyya,",
  "kv22.6:2.23_0": "cetiyaṁ vandeyya,",
  "kv22.6:2.24_0": "cetiye mālaṁ āropeyya,",
  "kv22.6:2.25_0": "gandhaṁ āropeyya,",
  "kv22.6:2.26_0": "vilepanaṁ āropeyya …pe…",
  "kv22.6:2.27_0": "cetiyaṁ abhidakkhiṇaṁ kareyyāti?",
  "kv22.6:2.28_0": "Āmantā.",
  "kv22.6:2.29_0": "Hañci supinantena cetiyaṁ abhidakkhiṇaṁ kareyya, no ca vata re vattabbe—",
  "kv22.6:2.30_0": "“sabbaṁ supinagatassa cittaṁ abyākatan”ti.",
  "kv22.6:3.1_0": "Na vattabbaṁ—",
  "kv22.6:3.2_0": "“sabbaṁ supinagatassa cittaṁ abyākatan”ti?",
  "kv22.6:3.3_0": "Āmantā.",
  "kv22.6:3.4_0": "Nanu supinagatassa cittaṁ abbohāriyaṁ vuttaṁ bhagavatāti?",
  "kv22.6:3.5_0": "Āmantā.",
  "kv22.6:3.6_0": "Hañci supinagatassa cittaṁ abbohāriyaṁ vuttaṁ bhagavatā, tena vata re vattabbe—",
  "kv22.6:3.7_0": "“sabbaṁ supinagatassa cittaṁ abyākatan”ti.",
  "kv22.6:3.8_0": "Abyākatakathā niṭṭhitā."
}