{
  "tha-ap483:0.1_0": "Therāpadāna",
  "tha-ap483:0.2_0": "Paṁsukūlavagga",
  "tha-ap483:0.3_0": "1. Paṁsukūlasaññakattheraapadāna",
  "tha-ap483:1.1_0": "“Tisso nāmāsi bhagavā,",
  "tha-ap483:1.2_0": "sayambhū aggapuggalo;",
  "tha-ap483:1.3_0": "Paṁsukūlaṁ ṭhapetvāna,",
  "tha-ap483:1.4_0": "vihāraṁ pāvisī jino.",
  "tha-ap483:2.1_0": "Vinataṁ dhanumādāya,",
  "tha-ap483:2.2_0": "bhakkhatthāya cariṁ ahaṁ;",
  "tha-ap483:2.3_0": "Maṇḍalaggaṁ gahetvāna,",
  "tha-ap483:2.4_0": "kānanaṁ pāvisiṁ ahaṁ.",
  "tha-ap483:3.1_0": "Tatthaddasaṁ paṁsukūlaṁ,",
  "tha-ap483:3.2_0": "dumagge laggitaṁ tadā;",
  "tha-ap483:3.3_0": "Cāpaṁ tattheva nikkhippa,",
  "tha-ap483:3.4_0": "sire katvāna añjaliṁ.",
  "tha-ap483:4.1_0": "Pasannacitto sumano,",
  "tha-ap483:4.2_0": "vipulāya ca pītiyā;",
  "tha-ap483:4.3_0": "Buddhaseṭṭhaṁ saritvāna,",
  "tha-ap483:4.4_0": "paṁsukūlaṁ avandahaṁ.",
  "tha-ap483:5.1_0": "Dvenavute ito kappe,",
  "tha-ap483:5.2_0": "paṁsukūlamavandahaṁ;",
  "tha-ap483:5.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap483:5.4_0": "vandanāya idaṁ phalaṁ.",
  "tha-ap483:6.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap483:6.2_0": "bhavā sabbe samūhatā;",
  "tha-ap483:6.3_0": "Nāgova bandhanaṁ chetvā,",
  "tha-ap483:6.4_0": "viharāmi anāsavo.",
  "tha-ap483:7.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap483:7.2_0": "mama buddhassa santike;",
  "tha-ap483:7.3_0": "Tisso vijjā anuppattā,",
  "tha-ap483:7.4_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap483:8.1_0": "Paṭisambhidā catasso,",
  "tha-ap483:8.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap483:8.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap483:8.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap483:9.1_0": "Itthaṁ sudaṁ āyasmā paṁsukūlasaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap483:10.1_0": "Paṁsukūlasaññakattherassāpadānaṁ paṭhamaṁ."
}