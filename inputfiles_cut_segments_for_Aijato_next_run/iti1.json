{
  "iti1:0.1_0": "Itivuttaka 1",
  "iti1:0.2_0": "Ekakanipāta",
  "iti1:0.3_0": "Paṭhamavagga",
  "iti1:0.4_0": "Lobhasutta",
  "iti1:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti1:2.1_0": "“Ekadhammaṁ, bhikkhave, pajahatha;",
  "iti1:2.2_0": "ahaṁ vo pāṭibhogo anāgāmitāya.",
  "iti1:2.3_0": "Katamaṁ ekadhammaṁ?",
  "iti1:2.4_0": "Lobhaṁ, bhikkhave, ekadhammaṁ pajahatha;",
  "iti1:2.5_0": "ahaṁ vo pāṭibhogo anāgāmitāyā”ti.",
  "iti1:2.6_0": "Etamatthaṁ bhagavā avoca.",
  "iti1:2.7_0": "Tatthetaṁ iti vuccati:",
  "iti1:3.1_0": "“Yena lobhena luddhāse,",
  "iti1:3.2_0": "sattā gacchanti duggatiṁ;",
  "iti1:3.3_0": "Taṁ lobhaṁ sammadaññāya,",
  "iti1:3.4_0": "pajahanti vipassino;",
  "iti1:3.5_0": "Pahāya na punāyanti,",
  "iti1:3.6_0": "imaṁ lokaṁ kudācanan”ti.",
  "iti1:4.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti1:4.2_0": "Paṭhamaṁ."
}