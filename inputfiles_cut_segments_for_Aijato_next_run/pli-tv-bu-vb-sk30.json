{
  "pli-tv-bu-vb-sk30:0.1_0": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk30:0.2_0": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk30:0.3_0": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk30:0.4_0": "Khambhakatavagga",
  "pli-tv-bu-vb-sk30:0.5_0": "30. Samatittikasikkhāpada",
  "pli-tv-bu-vb-sk30:1.1_0": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk30:1.2_0": "Tena kho pana samayena chabbaggiyā bhikkhū thūpīkataṁ piṇḍapātaṁ paṭiggaṇhanti …pe….",
  "pli-tv-bu-vb-sk30:1.3.1_0": "“Samatittikaṁ piṇḍapātaṁ paṭiggahessāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk30:1.4_0": "Samatittiko piṇḍapāto paṭiggahetabbo.",
  "pli-tv-bu-vb-sk30:1.5_0": "Yo anādariyaṁ paṭicca thūpīkataṁ piṇḍapātaṁ paṭiggaṇhāti, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk30:1.6.1_0": "Anāpatti—",
  "pli-tv-bu-vb-sk30:1.7.1_0": "asañcicca,",
  "pli-tv-bu-vb-sk30:1.7.2_0": "assatiyā,",
  "pli-tv-bu-vb-sk30:1.7.3_0": "ajānantassa,",
  "pli-tv-bu-vb-sk30:1.7.4_0": "āpadāsu,",
  "pli-tv-bu-vb-sk30:1.8_0": "ummattakassa,",
  "pli-tv-bu-vb-sk30:1.9_0": "ādikammikassāti.",
  "pli-tv-bu-vb-sk30:1.10_0": "Dasamasikkhāpadaṁ niṭṭhitaṁ.",
  "pli-tv-bu-vb-sk30:1.11_0": "Khambhakatavaggo tatiyo."
}