{
  "ja70:0.1_0": "Jātaka",
  "ja70:0.2_0": "Ekakanipāta",
  "ja70:0.3_0": "Itthivagga",
  "ja70:0.4_0": "10. Kuddālajātaka",
  "ja70:1.1_0": "“Na taṁ jitaṁ sādhu jitaṁ,",
  "ja70:1.2_0": "yaṁ jitaṁ avajīyati;",
  "ja70:1.3_0": "Taṁ kho jitaṁ sādhu jitaṁ,",
  "ja70:1.4_0": "yaṁ jitaṁ nāvajīyatī”ti.",
  "ja70:2.1_0": "Kuddālajātakaṁ dasamaṁ.",
  "ja70:2.2_0": "Itthivaggo sattamo.",
  "ja70:3.1_0": "Tassuddānaṁ",
  "ja70:4.1_0": "Sikhīsabbaghasopi ca vīṇavaro,",
  "ja70:4.2_0": "Pisuṇā mittabhedikā nandī nadī;",
  "ja70:4.3_0": "Mudulakkhaṇa sodariyā ca mano,",
  "ja70:4.4_0": "Visa sādhujitena bhavanti dasāti."
}