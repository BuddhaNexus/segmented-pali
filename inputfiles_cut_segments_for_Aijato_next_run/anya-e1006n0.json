{
  "anya-e1006n0:0_0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1006n0:1_0": "Suttantanīti",
  "anya-e1006n0:2_0": "1.",
  "anya-e1006n0:3_0": "Parā bhavantaṁ purisaṁ,",
  "anya-e1006n0:4_0": "Mayaṁ pucchāma gotamaṁ;",
  "anya-e1006n0:5_0": "Bhavantaṁ puṭṭhu māgamma,",
  "anya-e1006n0:6_0": "Kiṁ parābhavato mukhaṁ.",
  "anya-e1006n0:7_0": "2.",
  "anya-e1006n0:8_0": "Suvijāno bhavaṁhoti,",
  "anya-e1006n0:9_0": "Duvijāno parābhavo;",
  "anya-e1006n0:10_0": "Dhammakāmo bhavaṁ hoti,",
  "anya-e1006n0:11_0": "Dhammadessī parābhavo.",
  "anya-e1006n0:12_0": "3.",
  "anya-e1006n0:13_0": "Itihe taṁ vijānāma,",
  "anya-e1006n0:14_0": "Pathamo so parābhavo;",
  "anya-e1006n0:15_0": "Dutīyaṁ bhagavā brūhi,",
  "anya-e1006n0:16_0": "Kiṁ parābhavato mukhaṁ.",
  "anya-e1006n0:17_0": "4.",
  "anya-e1006n0:18_0": "Asantassa piyo hoti,",
  "anya-e1006n0:19_0": "Sante na kurute piyaṁ;",
  "anya-e1006n0:20_0": "Asataṁ dhammaṁ roceti,",
  "anya-e1006n0:21_0": "Taṁ parābhavato mukhaṁ.",
  "anya-e1006n0:22_0": "Kammāparādhasattānaṁ,",
  "anya-e1006n0:23_0": "Vināse paccupaṭṭhite;",
  "anya-e1006n0:24_0": "Anayo nayarūpena,",
  "anya-e1006n0:25_0": "Buddhimākamya tiṭṭhati.",
  "anya-e1006n0:26_0": "5.",
  "anya-e1006n0:27_0": "Niddāsīlī sabhāsīlī,",
  "anya-e1006n0:28_0": "Anuṭṭhātā ca yo naro;",
  "anya-e1006n0:29_0": "Alaso kodhapaññāṇo,",
  "anya-e1006n0:30_0": "Taṁ parābhavato mukhaṁ.",
  "anya-e1006n0:31_0": "6.",
  "anya-e1006n0:32_0": "Yo mātaraṁ pitaraṁ vā,",
  "anya-e1006n0:33_0": "Jiṇṇakaṁ gatayobbanaṁ;",
  "anya-e1006n0:34_0": "Pahusanto na bharati,",
  "anya-e1006n0:35_0": "Taṁ parābhavato mukhaṁ.",
  "anya-e1006n0:36_0": "7.",
  "anya-e1006n0:37_0": "Yo brāhmaṇaṁ samaṇaṁ vā,",
  "anya-e1006n0:38_0": "Aññaṁ vāpi vaṇibbakaṁ;",
  "anya-e1006n0:39_0": "Musāvādena vañceti,",
  "anya-e1006n0:40_0": "Taṁ parābhavato mukhaṁ.",
  "anya-e1006n0:41_0": "8.",
  "anya-e1006n0:42_0": "Pahutavitto puriso,",
  "anya-e1006n0:43_0": "Sahirañño sabhojano;",
  "anya-e1006n0:44_0": "Eko bhuñjati sādūni,",
  "anya-e1006n0:45_0": "Taṁ parābhavato mukhaṁ.",
  "anya-e1006n0:46_0": "9.",
  "anya-e1006n0:47_0": "Jātithaddho dhanathaddho,",
  "anya-e1006n0:48_0": "Gottathaddho ca yo naro;",
  "anya-e1006n0:49_0": "Sañātiṁ atimaññeti,",
  "anya-e1006n0:50_0": "Taṁ parābhavato mukhaṁ.",
  "anya-e1006n0:51_0": "10.",
  "anya-e1006n0:52_0": "Itthidhutto surādhutto,",
  "anya-e1006n0:53_0": "Akkhadhutto ca yo naro;",
  "anya-e1006n0:54_0": "Laddhaṁladdhaṁ vināseti,",
  "anya-e1006n0:55_0": "Taṁ parābhavato mukhaṁ.",
  "anya-e1006n0:56_0": "Na bāḷhaṁ itthiṁ gaccheyya,",
  "anya-e1006n0:57_0": "Sampassaṁ tejasaṅkhayaṁ;",
  "anya-e1006n0:58_0": "Kāsaṁ sāsaṁ daraṁ bālyaṁ,",
  "anya-e1006n0:59_0": "Khīṇamedo nigacchati.",
  "anya-e1006n0:60_0": "(Ka)",
  "anya-e1006n0:61_0": "Māyācetā marīcī ca,",
  "anya-e1006n0:62_0": "Soko rogo upaddavo;",
  "anya-e1006n0:63_0": "Kharā ca bandhanācetā,",
  "anya-e1006n0:64_0": "Maccupāso guhāsayo.",
  "anya-e1006n0:65_0": "(Kha)",
  "anya-e1006n0:66_0": "Balavanto dubbalā honti,",
  "anya-e1006n0:67_0": "Thāmavantopi hāyare;",
  "anya-e1006n0:68_0": "Cakkhumā andhakā honti,",
  "anya-e1006n0:69_0": "Mātugāmavasaṁgatā.",
  "anya-e1006n0:70_0": "(Ga)",
  "anya-e1006n0:71_0": "Guṇavanto nigguṇā honti,",
  "anya-e1006n0:72_0": "Paññavantopi hāyare;",
  "anya-e1006n0:73_0": "Pamuttā bandhanā senti,",
  "anya-e1006n0:74_0": "Mātugāmavasaṁgatā.",
  "anya-e1006n0:75_0": "(Gha)",
  "anya-e1006n0:76_0": "Yasaṁ kittiṁ dhitiṁ sūraṁ;",
  "anya-e1006n0:77_0": "Bāhussaccaṁ pajānanaṁ;",
  "anya-e1006n0:78_0": "Hāpayanti pamattassa;",
  "anya-e1006n0:79_0": "Kaṭṭhapuñcaṁva pāvako.",
  "anya-e1006n0:80_0": "11.",
  "anya-e1006n0:81_0": "Sehi dārehi santuṭṭho,",
  "anya-e1006n0:82_0": "Vesiyāsu padussati;",
  "anya-e1006n0:83_0": "Dussati paradāresu,",
  "anya-e1006n0:84_0": "Taṁ parābhavato mukhaṁ.",
  "anya-e1006n0:85_0": "(Ka)",
  "anya-e1006n0:86_0": "Mayañca bhariyaṁ nātikkamāma,",
  "anya-e1006n0:87_0": "Amheca bhariyā nātikkamanti;",
  "anya-e1006n0:88_0": "Aññatra tāhi brahmacariyaṁ carāma,",
  "anya-e1006n0:89_0": "Tasmā hi amhaṁ daharā na miyyare.",
  "anya-e1006n0:90_0": "(Kha)",
  "anya-e1006n0:91_0": "Etāsu jāyare suttamāsu,",
  "anya-e1006n0:92_0": "Medhāvino honti pahutapaññā;",
  "anya-e1006n0:93_0": "Bahussutā theraguṇā ca honti,",
  "anya-e1006n0:94_0": "Tasmā hi amhaṁ daharā na miyyare.",
  "anya-e1006n0:95_0": "12.",
  "anya-e1006n0:96_0": "Atītayobbano poso,",
  "anya-e1006n0:97_0": "Āneti timbarutthaniṁ;",
  "anya-e1006n0:98_0": "Tassa issā na supati,",
  "anya-e1006n0:99_0": "Taṁ parābhavato mukhaṁ.",
  "anya-e1006n0:100_0": "(Ka)",
  "anya-e1006n0:101_0": "Na dukkhaṁ ahinā daṭṭhaṁ,",
  "anya-e1006n0:102_0": "Na dukkhaṁ sattiyā hataṁ;",
  "anya-e1006n0:103_0": "Tañca dukkhañca tibbañca,",
  "anya-e1006n0:104_0": "Yaṁ passe jiṇṇakaṁ patiṁ.",
  "anya-e1006n0:105_0": "(Kha)",
  "anya-e1006n0:106_0": "Natthi khiṭṭā natthi rati,",
  "anya-e1006n0:107_0": "Jiṇṇena patinā saha;",
  "anya-e1006n0:108_0": "Natthi allāpasallāpo,",
  "anya-e1006n0:109_0": "Jagghituṁpi na sobhati.",
  "anya-e1006n0:110_0": "(Ga)",
  "anya-e1006n0:111_0": "Yadā ca daharo daharī,",
  "anya-e1006n0:112_0": "Mantayiṁsu rahogatā;",
  "anya-e1006n0:113_0": "Sabbe sokā vinassanti,",
  "anya-e1006n0:114_0": "Yekeci hadayassitā.",
  "anya-e1006n0:115_0": "13.",
  "anya-e1006n0:116_0": "Itthiṁ soṇḍiṁ vikiraṇiṁ,",
  "anya-e1006n0:117_0": "Purisaṁ vāpi tādisaṁ;",
  "anya-e1006n0:118_0": "Issariyasmiṁ thapeti,",
  "anya-e1006n0:119_0": "Taṁ parābhavato mukhaṁ.",
  "anya-e1006n0:120_0": "14.",
  "anya-e1006n0:121_0": "Appabhogo mahātaṇho,",
  "anya-e1006n0:122_0": "Khattiye jāyate kule;",
  "anya-e1006n0:123_0": "So ca rajjaṁ patthayati,",
  "anya-e1006n0:124_0": "Taṁ parābhavato mukhaṁ.",
  "anya-e1006n0:125_0": "“Suvijāno bhavaṁhoti,",
  "anya-e1006n0:126_0": "Duvijāno parābhavo”;",
  "anya-e1006n0:127_0": "Appabhogo mahātaṇho.",
  "anya-e1006n0:128_0": "15.",
  "anya-e1006n0:129_0": "Ete parābhave loke,",
  "anya-e1006n0:130_0": "Paṇḍito samavekkhiya;",
  "anya-e1006n0:131_0": "Ariyo dassanasampanno,",
  "anya-e1006n0:132_0": "Sa lokaṁ bhajate sivaṁ."
}