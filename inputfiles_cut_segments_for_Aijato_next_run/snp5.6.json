{
  "snp5.6:0.1_0": "Sutta Nipāta 5.6",
  "snp5.6:0.2_0": "Dhotakamāṇavapucchā",
  "snp5.6:1.1_0": "“Pucchāmi taṁ bhagavā brūhi metaṁ,",
  "snp5.6:1.2_0": "(iccāyasmā dhotako)",
  "snp5.6:1.3_0": "Vācābhikaṅkhāmi mahesi tuyhaṁ;",
  "snp5.6:1.4_0": "Tava sutvāna nigghosaṁ,",
  "snp5.6:1.5_0": "Sikkhe nibbānamattano”.",
  "snp5.6:2.1_0": "“Tenahātappaṁ karohi,",
  "snp5.6:2.2_0": "(dhotakāti bhagavā)",
  "snp5.6:2.3_0": "Idheva nipako sato;",
  "snp5.6:2.4_0": "Ito sutvāna nigghosaṁ,",
  "snp5.6:2.5_0": "Sikkhe nibbānamattano”.",
  "snp5.6:3.1_0": "“Passāmahaṁ devamanussaloke,",
  "snp5.6:3.2_0": "Akiñcanaṁ brāhmaṇamiriyamānaṁ;",
  "snp5.6:3.3_0": "Taṁ taṁ namassāmi samantacakkhu,",
  "snp5.6:3.4_0": "Pamuñca maṁ sakka kathaṅkathāhi”.",
  "snp5.6:4.1_0": "“Nāhaṁ sahissāmi pamocanāya,",
  "snp5.6:4.2_0": "Kathaṅkathiṁ dhotaka kañci loke;",
  "snp5.6:4.3_0": "Dhammañca seṭṭhaṁ abhijānamāno,",
  "snp5.6:4.4_0": "Evaṁ tuvaṁ oghamimaṁ taresi”.",
  "snp5.6:5.1_0": "“Anusāsa brahme karuṇāyamāno,",
  "snp5.6:5.2_0": "Vivekadhammaṁ yamahaṁ vijaññaṁ;",
  "snp5.6:5.3_0": "Yathāhaṁ ākāsova abyāpajjamāno,",
  "snp5.6:5.4_0": "Idheva santo asito careyyaṁ”.",
  "snp5.6:6.1_0": "“Kittayissāmi te santiṁ,",
  "snp5.6:6.2_0": "(dhotakāti bhagavā)",
  "snp5.6:6.3_0": "Diṭṭhe dhamme anītihaṁ;",
  "snp5.6:6.4_0": "Yaṁ viditvā sato caraṁ,",
  "snp5.6:6.5_0": "Tare loke visattikaṁ”.",
  "snp5.6:7.1_0": "“Tañcāhaṁ abhinandāmi,",
  "snp5.6:7.2_0": "mahesi santimuttamaṁ;",
  "snp5.6:7.3_0": "Yaṁ viditvā sato caraṁ,",
  "snp5.6:7.4_0": "tare loke visattikaṁ”.",
  "snp5.6:8.1_0": "“Yaṁ kiñci sampajānāsi,",
  "snp5.6:8.2_0": "(dhotakāti bhagavā)",
  "snp5.6:8.3_0": "Uddhaṁ adho tiriyañcāpi majjhe;",
  "snp5.6:8.4_0": "Etaṁ viditvā saṅgoti loke,",
  "snp5.6:8.5_0": "Bhavābhavāya mākāsi taṇhan”ti.",
  "snp5.6:8.6_0": "Dhotakamāṇavapucchā pañcamī."
}