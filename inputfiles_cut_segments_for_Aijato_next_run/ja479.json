{
  "ja479:0.1_0": "Jātaka",
  "ja479:0.2_0": "Terasakanipāta",
  "ja479:0.3_0": "Ambavagga",
  "ja479:0.4_0": "6. Kāliṅgabodhijātaka",
  "ja479:1.1_0": "Rājā kāliṅgo cakkavatti,",
  "ja479:1.2_0": "Dhammena pathavimanusāsaṁ;",
  "ja479:1.3_0": "Agamā bodhisamīpaṁ,",
  "ja479:1.4_0": "Nāgena mahānubhāvena.",
  "ja479:2.1_0": "Kāliṅgo bhāradvājo ca,",
  "ja479:2.2_0": "Rājānaṁ kāliṅgaṁ samaṇakolaññaṁ;",
  "ja479:2.3_0": "Cakkaṁ vattayato pariggahetvā,",
  "ja479:2.4_0": "Pañjalī idamavoca.",
  "ja479:3.1_0": "“Paccoroha mahārāja,",
  "ja479:3.2_0": "Bhūmibhāgo yathā samaṇuggato;",
  "ja479:3.3_0": "Idha anadhivarā buddhā,",
  "ja479:3.4_0": "Abhisambuddhā virocanti.",
  "ja479:4.1_0": "Padakkhiṇato āvaṭṭā,",
  "ja479:4.2_0": "Tiṇalatā asmiṁ bhūmibhāgasmiṁ;",
  "ja479:4.3_0": "Pathaviyā nābhiyaṁ maṇḍo,",
  "ja479:4.4_0": "Iti no sutaṁ mante mahārāja.",
  "ja479:5.1_0": "Sāgarapariyantāya,",
  "ja479:5.2_0": "Mediniyā sabbabhūtadharaṇiyā;",
  "ja479:5.3_0": "Pathaviyā ayaṁ maṇḍo,",
  "ja479:5.4_0": "Orohitvā namo karohi.",
  "ja479:6.1_0": "Ye te bhavanti nāgā ca,",
  "ja479:6.2_0": "abhijātā ca kuñjarā;",
  "ja479:6.3_0": "Ettāvatā padesaṁ te,",
  "ja479:6.4_0": "nāgā neva mupayanti.",
  "ja479:7.1_0": "Abhijāto nāgo kāmaṁ,",
  "ja479:7.2_0": "Pesehi kuñjaraṁ dantiṁ;",
  "ja479:7.3_0": "Ettāvatā padeso,",
  "ja479:7.4_0": "Sakkā nāgena mupagantuṁ.",
  "ja479:8.1_0": "Taṁ sutvā rājā kāliṅgo,",
  "ja479:8.2_0": "Veyyañjanikavaco nisāmetvā;",
  "ja479:8.3_0": "Sampesesi nāgaṁ ñassāma,",
  "ja479:8.4_0": "Mayaṁ yathimassidaṁ vacanaṁ”.",
  "ja479:9.1_0": "Sampesito ca raññā,",
  "ja479:9.2_0": "Nāgo koñcova abhinaditvāna;",
  "ja479:9.3_0": "Paṭisakkitvā nisīdi,",
  "ja479:9.4_0": "Garuṁva bhāraṁ asahamāno.",
  "ja479:10.1_0": "Kāliṅgabhāradvājo,",
  "ja479:10.2_0": "Nāgaṁ khīṇāyukaṁ viditvāna;",
  "ja479:10.3_0": "Rājānaṁ kāliṅgaṁ,",
  "ja479:10.4_0": "Taramāno ajjhabhāsittha;",
  "ja479:10.5_0": "“Aññaṁ saṅkama nāgaṁ,",
  "ja479:10.6_0": "Nāgo khīṇāyuko mahārāja”.",
  "ja479:11.1_0": "Taṁ sutvā kāliṅgo,",
  "ja479:11.2_0": "taramāno saṅkamī nāgaṁ;",
  "ja479:11.3_0": "Saṅkanteva raññe,",
  "ja479:11.4_0": "nāgo tattheva pati bhumyā;",
  "ja479:11.5_0": "Veyyañjanikavaco,",
  "ja479:11.6_0": "yathā tathā ahu nāgo.",
  "ja479:12.1_0": "Kāliṅgo rājā kāliṅgaṁ,",
  "ja479:12.2_0": "brāhmaṇaṁ etadavoca;",
  "ja479:12.3_0": "“Tvameva asi sambuddho,",
  "ja479:12.4_0": "sabbaññū sabbadassāvī”.",
  "ja479:13.1_0": "Taṁ anadhivāsento kāliṅgaṁ,",
  "ja479:13.2_0": "Brāhmaṇo idamavoca;",
  "ja479:13.3_0": "“Veyyañjanikā hi mayaṁ,",
  "ja479:13.4_0": "Buddhā sabbaññuno mahārāja.",
  "ja479:14.1_0": "Sabbaññū sabbavidū ca,",
  "ja479:14.2_0": "Buddhā na lakkhaṇena jānanti;",
  "ja479:14.3_0": "Āgamabalasā hi mayaṁ,",
  "ja479:14.4_0": "Buddhā sabbaṁ pajānanti”.",
  "ja479:15.1_0": "Mahayitvā sambodhiṁ,",
  "ja479:15.2_0": "Nānāturiyehi vajjamānehi;",
  "ja479:15.3_0": "Mālāvilepanaṁ abhiharitvā,",
  "ja479:15.4_0": "Atha rājā manupāyāsi.",
  "ja479:16.1_0": "Saṭṭhi vāhasahassāni,",
  "ja479:16.2_0": "pupphānaṁ sannipātayi;",
  "ja479:16.3_0": "Pūjesi rājā kāliṅgo,",
  "ja479:16.4_0": "bodhimaṇḍamanuttaranti.",
  "ja479:17.1_0": "Kāliṅgabodhijātakaṁ chaṭṭhaṁ."
}