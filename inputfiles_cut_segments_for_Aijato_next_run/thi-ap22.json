{
  "thi-ap22:0.1_0": "Therīapadāna",
  "thi-ap22:0.2_0": "Kuṇḍalakesīvagga",
  "thi-ap22:0.3_0": "2. Kisāgotamītherīapadāna",
  "thi-ap22:1.1_0": "“Padumuttaro nāma jino,",
  "thi-ap22:1.2_0": "sabbadhammāna pāragū;",
  "thi-ap22:1.3_0": "Ito satasahassamhi,",
  "thi-ap22:1.4_0": "kappe uppajji nāyako.",
  "thi-ap22:2.1_0": "Tadāhaṁ haṁsavatiyaṁ,",
  "thi-ap22:2.2_0": "jātā aññatare kule;",
  "thi-ap22:2.3_0": "Upetvā taṁ naravaraṁ,",
  "thi-ap22:2.4_0": "saraṇaṁ samupāgamiṁ.",
  "thi-ap22:3.1_0": "Dhammañca tassa assosiṁ,",
  "thi-ap22:3.2_0": "catusaccūpasañhitaṁ;",
  "thi-ap22:3.3_0": "Madhuraṁ paramassādaṁ,",
  "thi-ap22:3.4_0": "vaṭṭasantisukhāvahaṁ.",
  "thi-ap22:4.1_0": "Tadā ca bhikkhuniṁ vīro,",
  "thi-ap22:4.2_0": "lūkhacīvaradhāriniṁ;",
  "thi-ap22:4.3_0": "Ṭhapento etadaggamhi,",
  "thi-ap22:4.4_0": "vaṇṇayī purisuttamo.",
  "thi-ap22:5.1_0": "Janetvānappakaṁ pītiṁ,",
  "thi-ap22:5.2_0": "sutvā bhikkhuniyā guṇe;",
  "thi-ap22:5.3_0": "Kāraṁ katvāna buddhassa,",
  "thi-ap22:5.4_0": "yathāsatti yathābalaṁ.",
  "thi-ap22:6.1_0": "Nipacca munivaraṁ taṁ,",
  "thi-ap22:6.2_0": "taṁ ṭhānamabhipatthayiṁ;",
  "thi-ap22:6.3_0": "Tadānumodi sambuddho,",
  "thi-ap22:6.4_0": "ṭhānalābhāya nāyako.",
  "thi-ap22:7.1_0": "‘Satasahassito kappe,",
  "thi-ap22:7.2_0": "okkākakulasambhavo;",
  "thi-ap22:7.3_0": "Gotamo nāma gottena,",
  "thi-ap22:7.4_0": "satthā loke bhavissati.",
  "thi-ap22:8.1_0": "Tassa dhammesu dāyādā,",
  "thi-ap22:8.2_0": "orasā dhammanimmitā;",
  "thi-ap22:8.3_0": "Kisāgotamī nāmena,",
  "thi-ap22:8.4_0": "hessati satthu sāvikā’.",
  "thi-ap22:9.1_0": "Taṁ sutvā muditā hutvā,",
  "thi-ap22:9.2_0": "yāvajīvaṁ tadā jinaṁ;",
  "thi-ap22:9.3_0": "Mettacittā paricariṁ,",
  "thi-ap22:9.4_0": "paccayehi vināyakaṁ.",
  "thi-ap22:10.1_0": "Tena kammena sukatena,",
  "thi-ap22:10.2_0": "cetanāpaṇidhīhi ca;",
  "thi-ap22:10.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "thi-ap22:10.4_0": "tāvatiṁsamagacchahaṁ.",
  "thi-ap22:11.1_0": "Imamhi bhaddake kappe,",
  "thi-ap22:11.2_0": "brahmabandhu mahāyaso;",
  "thi-ap22:11.3_0": "Kassapo nāma gottena,",
  "thi-ap22:11.4_0": "uppajji vadataṁ varo.",
  "thi-ap22:12.1_0": "Upaṭṭhāko mahesissa,",
  "thi-ap22:12.2_0": "tadā āsi narissaro;",
  "thi-ap22:12.3_0": "Kāsirājā kikī nāma,",
  "thi-ap22:12.4_0": "bārāṇasipuruttame.",
  "thi-ap22:13.1_0": "Pañcamī tassa dhītāsiṁ,",
  "thi-ap22:13.2_0": "dhammā nāmena vissutā;",
  "thi-ap22:13.3_0": "Dhammaṁ sutvā jinaggassa,",
  "thi-ap22:13.4_0": "pabbajjaṁ samarocayiṁ.",
  "thi-ap22:14.1_0": "Anujāni na no tāto,",
  "thi-ap22:14.2_0": "agāreva tadā mayaṁ;",
  "thi-ap22:14.3_0": "Vīsavassasahassāni,",
  "thi-ap22:14.4_0": "vicarimha atanditā.",
  "thi-ap22:15.1_0": "Komāribrahmacariyaṁ,",
  "thi-ap22:15.2_0": "rājakaññā sukhedhitā;",
  "thi-ap22:15.3_0": "Buddhopaṭṭhānaniratā,",
  "thi-ap22:15.4_0": "muditā satta dhītaro.",
  "thi-ap22:16.1_0": "Samaṇī samaṇaguttā ca,",
  "thi-ap22:16.2_0": "bhikkhunī bhikkhudāyikā;",
  "thi-ap22:16.3_0": "Dhammā ceva sudhammā ca,",
  "thi-ap22:16.4_0": "sattamī saṅghadāyikā.",
  "thi-ap22:17.1_0": "Khemā uppalavaṇṇā ca,",
  "thi-ap22:17.2_0": "paṭācārā ca kuṇḍalā;",
  "thi-ap22:17.3_0": "Ahañca dhammadinnā ca,",
  "thi-ap22:17.4_0": "visākhā hoti sattamī.",
  "thi-ap22:18.1_0": "Tehi kammehi sukatehi,",
  "thi-ap22:18.2_0": "cetanāpaṇidhīhi ca;",
  "thi-ap22:18.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "thi-ap22:18.4_0": "tāvatiṁsamagacchahaṁ.",
  "thi-ap22:19.1_0": "Pacchime ca bhave dāni,",
  "thi-ap22:19.2_0": "jātā seṭṭhikule ahaṁ;",
  "thi-ap22:19.3_0": "Duggate adhane naṭṭhe,",
  "thi-ap22:19.4_0": "gatā ca sadhanaṁ kulaṁ.",
  "thi-ap22:20.1_0": "Patiṁ ṭhapetvā sesā me,",
  "thi-ap22:20.2_0": "dessanti adhanā iti;",
  "thi-ap22:20.3_0": "Yadā ca pasūtā āsiṁ,",
  "thi-ap22:20.4_0": "sabbesaṁ dayitā tadā.",
  "thi-ap22:21.1_0": "Yadā so taruṇo bhaddo,",
  "thi-ap22:21.2_0": "komalako sukhedhito;",
  "thi-ap22:21.3_0": "Sapāṇamiva kanto me,",
  "thi-ap22:21.4_0": "tadā yamavasaṁ gato.",
  "thi-ap22:22.1_0": "Sokaṭṭādīnavadanā,",
  "thi-ap22:22.2_0": "assunettā rudammukhā;",
  "thi-ap22:22.3_0": "Mataṁ kuṇapamādāya,",
  "thi-ap22:22.4_0": "vilapantī gamāmahaṁ.",
  "thi-ap22:23.1_0": "Tadā ekena sandiṭṭhā,",
  "thi-ap22:23.2_0": "upetvābhisakkuttamaṁ;",
  "thi-ap22:23.3_0": "Avocaṁ ‘dehi bhesajjaṁ,",
  "thi-ap22:23.4_0": "puttasañjīvananti bho’.",
  "thi-ap22:24.1_0": "‘Na vijjante matā yasmiṁ,",
  "thi-ap22:24.2_0": "gehe siddhatthakaṁ tato;",
  "thi-ap22:24.3_0": "Āharā’ti jino āha,",
  "thi-ap22:24.4_0": "vinayopāyakovido.",
  "thi-ap22:25.1_0": "Tadā gamitvā sāvatthiṁ,",
  "thi-ap22:25.2_0": "na labhiṁ tādisaṁ gharaṁ;",
  "thi-ap22:25.3_0": "Kuto siddhatthakaṁ tasmā,",
  "thi-ap22:25.4_0": "tato laddhā satiṁ ahaṁ.",
  "thi-ap22:26.1_0": "Kuṇapaṁ chaḍḍayitvāna,",
  "thi-ap22:26.2_0": "upesiṁ lokanāyakaṁ;",
  "thi-ap22:26.3_0": "Dūratova mamaṁ disvā,",
  "thi-ap22:26.4_0": "avoca madhurassaro.",
  "thi-ap22:27.1_0": "‘Yo ca vassasataṁ jīve,",
  "thi-ap22:27.2_0": "apassaṁ udayabbayaṁ;",
  "thi-ap22:27.3_0": "Ekāhaṁ jīvitaṁ seyyo,",
  "thi-ap22:27.4_0": "passato udayabbayaṁ.",
  "thi-ap22:28.1_0": "Na gāmadhammo nigamassa dhammo,",
  "thi-ap22:28.2_0": "Na cāpiyaṁ ekakulassa dhammo;",
  "thi-ap22:28.3_0": "Sabbassa lokassa sadevakassa,",
  "thi-ap22:28.4_0": "Eseva dhammo yadidaṁ aniccatā’.",
  "thi-ap22:29.1_0": "Sāhaṁ sutvānimā gāthā,",
  "thi-ap22:29.2_0": "dhammacakkhuṁ visodhayiṁ;",
  "thi-ap22:29.3_0": "Tato viññātasaddhammā,",
  "thi-ap22:29.4_0": "pabbajiṁ anagāriyaṁ.",
  "thi-ap22:30.1_0": "Tathā pabbajitā santī,",
  "thi-ap22:30.2_0": "yuñjantī jinasāsane;",
  "thi-ap22:30.3_0": "Na cireneva kālena,",
  "thi-ap22:30.4_0": "arahattamapāpuṇiṁ.",
  "thi-ap22:31.1_0": "Iddhīsu ca vasī homi,",
  "thi-ap22:31.2_0": "dibbāya sotadhātuyā;",
  "thi-ap22:31.3_0": "Paracittāni jānāmi,",
  "thi-ap22:31.4_0": "satthusāsanakārikā.",
  "thi-ap22:32.1_0": "Pubbenivāsaṁ jānāmi,",
  "thi-ap22:32.2_0": "dibbacakkhu visodhitaṁ;",
  "thi-ap22:32.3_0": "Khepetvā āsave sabbe,",
  "thi-ap22:32.4_0": "visuddhāsiṁ sunimmalā.",
  "thi-ap22:33.1_0": "Pariciṇṇo mayā satthā,",
  "thi-ap22:33.2_0": "kataṁ buddhassa sāsanaṁ;",
  "thi-ap22:33.3_0": "Ohito garuko bhāro,",
  "thi-ap22:33.4_0": "bhavanetti samūhatā.",
  "thi-ap22:34.1_0": "Yassatthāya pabbajitā,",
  "thi-ap22:34.2_0": "agārasmānagāriyaṁ;",
  "thi-ap22:34.3_0": "So me attho anuppatto,",
  "thi-ap22:34.4_0": "sabbasaṁyojanakkhayo.",
  "thi-ap22:35.1_0": "Atthadhammaniruttīsu,",
  "thi-ap22:35.2_0": "paṭibhāne tatheva ca;",
  "thi-ap22:35.3_0": "Ñāṇaṁ me vimalaṁ suddhaṁ,",
  "thi-ap22:35.4_0": "buddhaseṭṭhassa vāhasā.",
  "thi-ap22:36.1_0": "Saṅkārakūṭā āhitvā,",
  "thi-ap22:36.2_0": "susānā rathiyāpi ca;",
  "thi-ap22:36.3_0": "Tato saṅghāṭikaṁ katvā,",
  "thi-ap22:36.4_0": "lūkhaṁ dhāremi cīvaraṁ.",
  "thi-ap22:37.1_0": "Jino tasmiṁ guṇe tuṭṭho,",
  "thi-ap22:37.2_0": "lūkhacīvaradhāraṇe;",
  "thi-ap22:37.3_0": "Ṭhapesi etadaggamhi,",
  "thi-ap22:37.4_0": "parisāsu vināyako.",
  "thi-ap22:38.1_0": "Kilesā jhāpitā mayhaṁ,",
  "thi-ap22:38.2_0": "…pe…",
  "thi-ap22:38.3_0": "viharāmi anāsavā.",
  "thi-ap22:39.1_0": "Svāgataṁ vata me āsi,",
  "thi-ap22:39.2_0": "…pe…",
  "thi-ap22:39.3_0": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap22:40.1_0": "Paṭisambhidā catasso,",
  "thi-ap22:40.2_0": "…pe…",
  "thi-ap22:40.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "thi-ap22:41.1_0": "Itthaṁ sudaṁ kisāgotamī bhikkhunī imā gāthāyo abhāsitthāti.",
  "thi-ap22:42.1_0": "Kisāgotamītheriyāpadānaṁ dutiyaṁ."
}