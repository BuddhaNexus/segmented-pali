{
  "tha-ap42:0.1_0": "Therāpadāna",
  "tha-ap42:0.2_0": "Kuṇḍadhānavagga",
  "tha-ap42:0.3_0": "10. Kapparukkhiyattheraapadāna",
  "tha-ap42:1.1_0": "“Siddhatthassa bhagavato,",
  "tha-ap42:1.2_0": "thūpaseṭṭhassa sammukhā;",
  "tha-ap42:1.3_0": "Vicittadusse lagetvā,",
  "tha-ap42:1.4_0": "kapparukkhaṁ ṭhapesahaṁ.",
  "tha-ap42:2.1_0": "Yaṁ yaṁ yonupapajjāmi,",
  "tha-ap42:2.2_0": "devattaṁ atha mānusaṁ;",
  "tha-ap42:2.3_0": "Sobhayanto mama dvāraṁ,",
  "tha-ap42:2.4_0": "kapparukkho patiṭṭhati.",
  "tha-ap42:3.1_0": "Ahañca parisā ceva,",
  "tha-ap42:3.2_0": "ye keci mama vassitā;",
  "tha-ap42:3.3_0": "Tamhā dussaṁ gahetvāna,",
  "tha-ap42:3.4_0": "nivāsema mayaṁ sadā.",
  "tha-ap42:4.1_0": "Catunnavutito kappe,",
  "tha-ap42:4.2_0": "yaṁ rukkhaṁ ṭhapayiṁ ahaṁ;",
  "tha-ap42:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap42:4.4_0": "kapparukkhassidaṁ phalaṁ.",
  "tha-ap42:5.1_0": "Ito ca sattame kappe,",
  "tha-ap42:5.2_0": "suceḷā aṭṭha khattiyā;",
  "tha-ap42:5.3_0": "Sattaratanasampannā,",
  "tha-ap42:5.4_0": "cakkavattī mahabbalā.",
  "tha-ap42:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap42:6.2_0": "…pe…",
  "tha-ap42:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap42:7.1_0": "Itthaṁ sudaṁ āyasmā kapparukkhiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap42:8.1_0": "Kapparukkhiyattherassāpadānaṁ dasamaṁ.",
  "tha-ap42:9.1_0": "Kuṇḍadhānavaggo catuttho.",
  "tha-ap42:10.1_0": "Tassuddānaṁ",
  "tha-ap42:11.1_0": "Kuṇḍasāgatakaccānā,",
  "tha-ap42:11.2_0": "udāyī mogharājako;",
  "tha-ap42:11.3_0": "Adhimutto lasuṇado,",
  "tha-ap42:11.4_0": "āyāgī dhammacakkiko;",
  "tha-ap42:11.5_0": "Kapparukkhī ca dasamo,",
  "tha-ap42:11.6_0": "gāthā dvayadasasataṁ."
}