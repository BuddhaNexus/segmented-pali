{
  "ja341:0.1_0": "Jātaka",
  "ja341:0.2_0": "Catukkanipāta",
  "ja341:0.3_0": "Cūḷakuṇālavagga",
  "ja341:0.4_0": "1. Kaṇḍarījātaka",
  "ja341:1.1_0": "Narānamārāmakarāsu nārisu,",
  "ja341:1.2_0": "Anekacittāsu aniggahāsu ca;",
  "ja341:1.3_0": "Sabbattha nāpītikarāpi ce siyā,",
  "ja341:1.4_0": "Na vissase titthasamā hi nāriyo.",
  "ja341:2.1_0": "“Yaṁ ve disvā kaṇḍarīkinnarānaṁ,",
  "ja341:2.2_0": "Sabbitthiyo na ramanti agāre;",
  "ja341:2.3_0": "Taṁ tādisaṁ maccaṁ cajitvā bhariyā,",
  "ja341:2.4_0": "Aññaṁ disvā purisaṁ pīṭhasappiṁ”.",
  "ja341:3.1_0": "“Bakassa ca bāvarikassa rañño,",
  "ja341:3.2_0": "Accantakāmānugatassa bhariyā;",
  "ja341:3.3_0": "Avācarī paṭṭhavasānugassa,",
  "ja341:3.4_0": "Kaṁ vāpi itthī nāticare tadaññaṁ”.",
  "ja341:4.1_0": "“Piṅgiyānī sabbalokissarassa,",
  "ja341:4.2_0": "Rañño piyā brahmadattassa bhariyā;",
  "ja341:4.3_0": "Avācarī paṭṭhavasānugassa,",
  "ja341:4.4_0": "Taṁ vāpi sā nājjhagā kāmakāminī”ti.",
  "ja341:5.1_0": "Kaṇḍarījātakaṁ paṭhamaṁ."
}