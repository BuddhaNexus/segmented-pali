{
  "ja124:0.1_0": "Jātaka",
  "ja124:0.2_0": "Ekakanipāta",
  "ja124:0.3_0": "Kusanāḷivagga",
  "ja124:0.4_0": "4. Ambajātaka",
  "ja124:1.1_0": "“Vāyametheva puriso,",
  "ja124:1.2_0": "na nibbindeyya paṇḍito;",
  "ja124:1.3_0": "Vāyāmassa phalaṁ passa,",
  "ja124:1.4_0": "bhuttā ambā anītihan”ti.",
  "ja124:2.1_0": "Ambajātakaṁ catutthaṁ."
}