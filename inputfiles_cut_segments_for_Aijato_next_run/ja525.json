{
  "ja525:0.1_0": "Jātaka",
  "ja525:0.2_0": "Cattālīsanipāta",
  "ja525:0.3_0": "Tesakuṇavagga",
  "ja525:0.4_0": "5 Cūḷasutasomajātaka",
  "ja525:1.1_0": "“Āmantayāmi nigamaṁ,",
  "ja525:1.2_0": "mittāmacce parissaje;",
  "ja525:1.3_0": "Sirasmiṁ palitaṁ jātaṁ,",
  "ja525:1.4_0": "pabbajjaṁ dāni rocahaṁ”.",
  "ja525:2.1_0": "“Abhumme kathaṁ nu bhaṇasi,",
  "ja525:2.2_0": "Sallaṁ me deva urasi kappesi;",
  "ja525:2.3_0": "Sattasatā te bhariyā,",
  "ja525:2.4_0": "Kathaṁ nu te tā bhavissanti”.",
  "ja525:3.1_0": "“Paññāyihinti etā,",
  "ja525:3.2_0": "Daharā aññampi tā gamissanti;",
  "ja525:3.3_0": "Saggañcassa patthayāno,",
  "ja525:3.4_0": "Tena ahaṁ pabbajissāmi”.",
  "ja525:4.1_0": "“Dulladdhaṁ me āsi sutasoma,",
  "ja525:4.2_0": "Yassa te homahaṁ mātā;",
  "ja525:4.3_0": "Yaṁ me vilapantiyā,",
  "ja525:4.4_0": "Anapekkho pabbajasi deva.",
  "ja525:5.1_0": "Dulladdhaṁ me āsi sutasoma,",
  "ja525:5.2_0": "Yaṁ taṁ ahaṁ vijāyissaṁ;",
  "ja525:5.3_0": "Yaṁ me vilapantiyā,",
  "ja525:5.4_0": "Anapekkho pabbajasi deva”.",
  "ja525:6.1_0": "“Ko nāmeso dhammo,",
  "ja525:6.2_0": "Sutasoma kā ca nāma pabbajjā;",
  "ja525:6.3_0": "Yaṁ no amhe jiṇṇe,",
  "ja525:6.4_0": "Anapekkho pabbajasi deva.",
  "ja525:7.1_0": "Puttāpi tuyhaṁ bahavo,",
  "ja525:7.2_0": "daharā appattayobbanā;",
  "ja525:7.3_0": "Mañjū tepi taṁ apassantā,",
  "ja525:7.4_0": "maññe dukkhaṁ nigacchanti”.",
  "ja525:8.1_0": "“Puttehi ca me etehi,",
  "ja525:8.2_0": "Daharehi appattayobbanehi;",
  "ja525:8.3_0": "Mañjūhi sabbehipi tumhehi,",
  "ja525:8.4_0": "Cirampi ṭhatvā vināsabhāvo”.",
  "ja525:9.1_0": "“Chinnaṁ nu tuyhaṁ hadayaṁ,",
  "ja525:9.2_0": "Adu te karuṇā ca natthi amhesu;",
  "ja525:9.3_0": "Yaṁ no vikandantiyo,",
  "ja525:9.4_0": "Anapekkho pabbajasi deva”.",
  "ja525:10.1_0": "“Na ca mayhaṁ chinnaṁ hadayaṁ,",
  "ja525:10.2_0": "Atthi karuṇāpi mayhaṁ tumhesu;",
  "ja525:10.3_0": "Saggañca patthayāno,",
  "ja525:10.4_0": "Tena ahaṁ pabbajissāmi”.",
  "ja525:11.1_0": "“Dulladdhaṁ me āsi,",
  "ja525:11.2_0": "Sutasoma yassa te ahaṁ bhariyā;",
  "ja525:11.3_0": "Yaṁ me vilapantiyā,",
  "ja525:11.4_0": "Anapekkho pabbajasi deva.",
  "ja525:12.1_0": "Dulladdhaṁ me āsi,",
  "ja525:12.2_0": "Sutasoma yassa te ahaṁ bhariyā;",
  "ja525:12.3_0": "Yaṁ me kucchipaṭisandhiṁ,",
  "ja525:12.4_0": "Anapekkho pabbajasi deva.",
  "ja525:13.1_0": "Paripakko me gabbho,",
  "ja525:13.2_0": "Kucchigato yāva naṁ vijāyāmi;",
  "ja525:13.3_0": "Māhaṁ ekā vidhavā,",
  "ja525:13.4_0": "Pacchā dukkhāni addakkhiṁ”.",
  "ja525:14.1_0": "“Paripakko te gabbho,",
  "ja525:14.2_0": "Kucchigato iṅgha tvaṁ vijāyassu;",
  "ja525:14.3_0": "Puttaṁ anomavaṇṇaṁ,",
  "ja525:14.4_0": "Taṁ hitvā pabbajissāmi”.",
  "ja525:15.1_0": "“Mā tvaṁ cande rudi,",
  "ja525:15.2_0": "Mā soci vanatimiramattakkhi;",
  "ja525:15.3_0": "Āroha varapāsādaṁ,",
  "ja525:15.4_0": "Anapekkho ahaṁ gamissāmi”.",
  "ja525:16.1_0": "“Ko taṁ amma kopesi,",
  "ja525:16.2_0": "Kiṁ rodasi pekkhasi ca maṁ bāḷhaṁ;",
  "ja525:16.3_0": "Kaṁ avajjhaṁ ghātemi,",
  "ja525:16.4_0": "Ñātīnaṁ udikkhamānānaṁ”.",
  "ja525:17.1_0": "“Na hi so sakkā hantuṁ,",
  "ja525:17.2_0": "Vijitāvī yo maṁ tāta kopesi;",
  "ja525:17.3_0": "Pitā te maṁ tāta avaca,",
  "ja525:17.4_0": "Anapekkho ahaṁ gamissāmi”.",
  "ja525:18.1_0": "“Yohaṁ pubbe niyyāmi,",
  "ja525:18.2_0": "Uyyānaṁ mattakuñjare ca yodhemi;",
  "ja525:18.3_0": "Sutasome pabbajite,",
  "ja525:18.4_0": "Kathaṁ nu dāni karissāmi”.",
  "ja525:19.1_0": "“Mātucca me rudantyā,",
  "ja525:19.2_0": "Jeṭṭhassa ca bhātuno akāmassa;",
  "ja525:19.3_0": "Hatthepi te gahessaṁ,",
  "ja525:19.4_0": "Na hi gacchasi no akāmānaṁ”.",
  "ja525:20.1_0": "“Uṭṭhehi tvaṁ dhāti,",
  "ja525:20.2_0": "Imaṁ kumāraṁ ramehi aññattha;",
  "ja525:20.3_0": "Mā me paripanthamakāsi,",
  "ja525:20.4_0": "Saggaṁ mama patthayānassa”.",
  "ja525:21.1_0": "“Yaṁnūnimaṁ dadeyyaṁ pabhaṅkaraṁ,",
  "ja525:21.2_0": "Ko nu me imināttho;",
  "ja525:21.3_0": "Sutasome pabbajite,",
  "ja525:21.4_0": "Kiṁ nu menaṁ karissāmi”.",
  "ja525:22.1_0": "“Koso ca tuyhaṁ vipulo,",
  "ja525:22.2_0": "Koṭṭhāgārañca tuyhaṁ paripūraṁ;",
  "ja525:22.3_0": "Pathavī ca tuyhaṁ vijitā,",
  "ja525:22.4_0": "Ramassu mā pabbaji deva”.",
  "ja525:23.1_0": "“Koso ca mayhaṁ vipulo,",
  "ja525:23.2_0": "Koṭṭhāgārañca mayhaṁ paripūraṁ;",
  "ja525:23.3_0": "Pathavī ca mayhaṁ vijitā,",
  "ja525:23.4_0": "Taṁ hitvā pabbajissāmi”.",
  "ja525:24.1_0": "“Mayhampi dhanaṁ pahūtaṁ,",
  "ja525:24.2_0": "Saṅkhātuṁ nopi deva sakkomi;",
  "ja525:24.3_0": "Taṁ te dadāmi sabbampi,",
  "ja525:24.4_0": "Ramassu mā pabbaji deva”.",
  "ja525:25.1_0": "“Jānāmi dhanaṁ pahūtaṁ,",
  "ja525:25.2_0": "Kulavaddhana pūjito tayā casmi;",
  "ja525:25.3_0": "Saggañca patthayāno,",
  "ja525:25.4_0": "Tena ahaṁ pabbajissāmi.",
  "ja525:26.1_0": "Ukkaṇṭhitosmi bāḷhaṁ,",
  "ja525:26.2_0": "Arati maṁ somadatta āvisati;",
  "ja525:26.3_0": "Bahukāpi me antarāyā,",
  "ja525:26.4_0": "Ajjevāhaṁ pabbajissāmi”.",
  "ja525:27.1_0": "“Idañca tuyhaṁ rucitaṁ,",
  "ja525:27.2_0": "Sutasoma ajjeva dāni tvaṁ pabbaja;",
  "ja525:27.3_0": "Ahampi pabbajissāmi,",
  "ja525:27.4_0": "Na ussahe tayā vinā ahaṁ ṭhātuṁ”.",
  "ja525:28.1_0": "“Na hi sakkā pabbajituṁ,",
  "ja525:28.2_0": "Nagare na hi paccati janapade ca;",
  "ja525:28.3_0": "Sutasome pabbajite,",
  "ja525:28.4_0": "Kathaṁ nu dāni karissāma.",
  "ja525:29.1_0": "Upanīyatidaṁ maññe,",
  "ja525:29.2_0": "Parittaṁ udakaṁva caṅkavāramhi;",
  "ja525:29.3_0": "Evaṁ suparittake jīvite,",
  "ja525:29.4_0": "Na ca pamajjituṁ kālo.",
  "ja525:30.1_0": "Upanīyatidaṁ maññe,",
  "ja525:30.2_0": "Parittaṁ udakaṁva caṅkavāramhi;",
  "ja525:30.3_0": "Evaṁ suparittake jīvite,",
  "ja525:30.4_0": "Andhabālā pamajjanti.",
  "ja525:31.1_0": "Te vaḍḍhayanti nirayaṁ,",
  "ja525:31.2_0": "Tiracchānayoniñca pettivisayañca;",
  "ja525:31.3_0": "Taṇhāya bandhanabaddhā,",
  "ja525:31.4_0": "Vaḍḍhenti asurakāyaṁ”.",
  "ja525:32.1_0": "“Ūhaññate rajaggaṁ,",
  "ja525:32.2_0": "Avidūre pubbakamhi ca pāsāde;",
  "ja525:32.3_0": "Maññe no kesā chinnā,",
  "ja525:32.4_0": "Yasassino dhammarājassa”.",
  "ja525:33.1_0": "“Ayamassa pāsādo,",
  "ja525:33.2_0": "Sovaṇṇapupphamālyavītikiṇṇo;",
  "ja525:33.3_0": "Yahimanuvicari rājā,",
  "ja525:33.4_0": "Parikiṇṇo itthāgārehi.",
  "ja525:34.1_0": "Ayamassa pāsādo,",
  "ja525:34.2_0": "Sovaṇṇapupphamālyavītikiṇṇo;",
  "ja525:34.3_0": "Yahimanuvicari rājā,",
  "ja525:34.4_0": "Parikiṇṇo ñātisaṅghena.",
  "ja525:35.1_0": "Idamassa kūṭāgāraṁ,",
  "ja525:35.2_0": "Sovaṇṇapupphamālyavītikiṇṇaṁ;",
  "ja525:35.3_0": "Yahimanuvicari rājā,",
  "ja525:35.4_0": "Parikiṇṇo itthāgārehi.",
  "ja525:36.1_0": "Idamassa kūṭāgāraṁ,",
  "ja525:36.2_0": "Sovaṇṇapupphamālyavītikiṇṇaṁ;",
  "ja525:36.3_0": "Yahimanuvicari rājā,",
  "ja525:36.4_0": "Parikiṇṇo ñātisaṅghena.",
  "ja525:37.1_0": "Ayamassa asokavanikā,",
  "ja525:37.2_0": "Supupphitā sabbakālikā rammā;",
  "ja525:37.3_0": "Yahimanuvicari rājā,",
  "ja525:37.4_0": "Parikiṇṇo itthāgārehi.",
  "ja525:38.1_0": "Ayamassa asokavanikā,",
  "ja525:38.2_0": "Supupphitā sabbakālikā rammā;",
  "ja525:38.3_0": "Yahimanuvicari rājā,",
  "ja525:38.4_0": "Parikiṇṇo ñātisaṅghena.",
  "ja525:39.1_0": "Idamassa uyyānaṁ,",
  "ja525:39.2_0": "Supupphitaṁ sabbakālikaṁ rammaṁ;",
  "ja525:39.3_0": "Yahimanuvicari rājā,",
  "ja525:39.4_0": "Parikiṇṇo itthāgārehi.",
  "ja525:40.1_0": "Idamassa uyyānaṁ,",
  "ja525:40.2_0": "Supupphitaṁ sabbakālikaṁ rammaṁ;",
  "ja525:40.3_0": "Yahimanuvicari rājā,",
  "ja525:40.4_0": "Parikiṇṇo ñātisaṅghena.",
  "ja525:41.1_0": "Idamassa kaṇikāravanaṁ,",
  "ja525:41.2_0": "Supupphitaṁ sabbakālikaṁ rammaṁ;",
  "ja525:41.3_0": "Yahimanuvicari rājā,",
  "ja525:41.4_0": "Parikiṇṇo itthāgārehi.",
  "ja525:42.1_0": "Idamassa kaṇikāravanaṁ,",
  "ja525:42.2_0": "Supupphitaṁ sabbakālikaṁ rammaṁ;",
  "ja525:42.3_0": "Yahimanuvicari rājā,",
  "ja525:42.4_0": "Parikiṇṇo ñātisaṅghena.",
  "ja525:43.1_0": "Idamassa pāṭalivanaṁ,",
  "ja525:43.2_0": "Supupphitaṁ sabbakālikaṁ rammaṁ;",
  "ja525:43.3_0": "Yahimanuvicari rājā,",
  "ja525:43.4_0": "Parikiṇṇo itthāgārehi.",
  "ja525:44.1_0": "Idamassa pāṭalivanaṁ,",
  "ja525:44.2_0": "Supupphitaṁ sabbakālikaṁ rammaṁ;",
  "ja525:44.3_0": "Yahimanuvicari rājā,",
  "ja525:44.4_0": "Parikiṇṇo ñātisaṅghena.",
  "ja525:45.1_0": "Idamassa ambavanaṁ,",
  "ja525:45.2_0": "Supupphitaṁ sabbakālikaṁ rammaṁ;",
  "ja525:45.3_0": "Yahimanuvicari rājā,",
  "ja525:45.4_0": "Parikiṇṇo itthāgārehi.",
  "ja525:46.1_0": "Idamassa ambavanaṁ,",
  "ja525:46.2_0": "Supupphitaṁ sabbakālikaṁ rammaṁ;",
  "ja525:46.3_0": "Yahimanuvicari rājā,",
  "ja525:46.4_0": "Parikiṇṇo ñātisaṅghena.",
  "ja525:47.1_0": "Ayamassa pokkharaṇī,",
  "ja525:47.2_0": "Sañchannā aṇḍajehi vītikiṇṇā;",
  "ja525:47.3_0": "Yahimanuvicari rājā,",
  "ja525:47.4_0": "Parikiṇṇo itthāgārehi.",
  "ja525:48.1_0": "Ayamassa pokkharaṇī,",
  "ja525:48.2_0": "Sañchannā aṇḍajehi vītikiṇṇā;",
  "ja525:48.3_0": "Yahimanuvicari rājā,",
  "ja525:48.4_0": "Parikiṇṇo ñātisaṅghena.",
  "ja525:49.1_0": "Rājā vo kho pabbajito,",
  "ja525:49.2_0": "Sutasomo rajjaṁ imaṁ pahatvāna;",
  "ja525:49.3_0": "Kāsāyavatthavasano,",
  "ja525:49.4_0": "Nāgova ekako carati”.",
  "ja525:50.1_0": "“Māssu pubbe ratikīḷitāni,",
  "ja525:50.2_0": "Hasitāni ca anussarittha;",
  "ja525:50.3_0": "Mā vo kāmā haniṁsu,",
  "ja525:50.4_0": "Rammaṁ hi sudassanaṁ nagaraṁ.",
  "ja525:51.1_0": "Mettacittañca bhāvetha,",
  "ja525:51.2_0": "Appamāṇaṁ divā ca ratto ca;",
  "ja525:51.3_0": "Agacchittha devapuraṁ,",
  "ja525:51.4_0": "Āvāsaṁ puññakamminan”ti.",
  "ja525:52.1_0": "Cūḷasutasomajātakaṁ pañcamaṁ.",
  "ja525:53.1_0": "Cattālīsanipātaṁ niṭṭhitaṁ.",
  "ja525:54.1_0": "Tassuddānaṁ",
  "ja525:55.1_0": "Suvapaṇḍitajambukakuṇḍalino,",
  "ja525:55.2_0": "Varakaññamalambusajātakañca;",
  "ja525:55.3_0": "Pavaruttamasaṅkhasirīvhayako,",
  "ja525:55.4_0": "Sutasomaarindhamarājavaro."
}