{
  "tha-ap327:0.1_0": "Therāpadāna",
  "tha-ap327:0.2_0": "Umāpupphiyavagga",
  "tha-ap327:0.3_0": "5. Nimittasaññakattheraapadāna",
  "tha-ap327:1.1_0": "“Candabhāgānadītīre,",
  "tha-ap327:1.2_0": "vasāmi assame ahaṁ;",
  "tha-ap327:1.3_0": "Suvaṇṇamigamaddakkhiṁ,",
  "tha-ap327:1.4_0": "carantaṁ vipine ahaṁ.",
  "tha-ap327:2.1_0": "Mige cittaṁ pasādetvā,",
  "tha-ap327:2.2_0": "lokajeṭṭhaṁ anussariṁ;",
  "tha-ap327:2.3_0": "Tena cittappasādena,",
  "tha-ap327:2.4_0": "aññe buddhe anussariṁ.",
  "tha-ap327:3.1_0": "Abbhatītā ca ye buddhā,",
  "tha-ap327:3.2_0": "vattamānā anāgatā;",
  "tha-ap327:3.3_0": "Evamevaṁ virocanti,",
  "tha-ap327:3.4_0": "migarājāva te tayo.",
  "tha-ap327:4.1_0": "Catunnavutito kappe,",
  "tha-ap327:4.2_0": "yaṁ saññamalabhiṁ tadā;",
  "tha-ap327:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap327:4.4_0": "buddhasaññāyidaṁ phalaṁ.",
  "tha-ap327:5.1_0": "Sattavīse ito kappe,",
  "tha-ap327:5.2_0": "eko āsiṁ mahīpati;",
  "tha-ap327:5.3_0": "Araññasattho nāmena,",
  "tha-ap327:5.4_0": "cakkavattī mahabbalo.",
  "tha-ap327:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap327:6.2_0": "…pe…",
  "tha-ap327:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap327:7.1_0": "Itthaṁ sudaṁ āyasmā nimittasaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap327:8.1_0": "Nimittasaññakattherassāpadānaṁ pañcamaṁ."
}