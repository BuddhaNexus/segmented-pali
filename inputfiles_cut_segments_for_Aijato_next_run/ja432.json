{
  "ja432:0.1_0": "Jātaka",
  "ja432:0.2_0": "Navakanipāta",
  "ja432:0.3_0": "Gijjhavagga",
  "ja432:0.4_0": "6. Padakusalamāṇavajātaka",
  "ja432:1.1_0": "“Bahussutaṁ cittakathiṁ,",
  "ja432:1.2_0": "gaṅgā vahati pāṭaliṁ;",
  "ja432:1.3_0": "Vuyhamānaka bhaddante,",
  "ja432:1.4_0": "ekaṁ me dehi gāthakaṁ”.",
  "ja432:2.1_0": "“Yena siñcanti dukkhitaṁ,",
  "ja432:2.2_0": "yena siñcanti āturaṁ;",
  "ja432:2.3_0": "Tassa majjhe marissāmi,",
  "ja432:2.4_0": "jātaṁ saraṇato bhayaṁ”.",
  "ja432:3.1_0": "“Yattha bījāni rūhanti,",
  "ja432:3.2_0": "sattā yattha patiṭṭhitā;",
  "ja432:3.3_0": "Sā me sīsaṁ nipīḷeti,",
  "ja432:3.4_0": "jātaṁ saraṇato bhayaṁ”.",
  "ja432:4.1_0": "“Yena bhattāni paccanti,",
  "ja432:4.2_0": "sītaṁ yena vihaññati;",
  "ja432:4.3_0": "So maṁ ḍahati gattāni,",
  "ja432:4.4_0": "jātaṁ saraṇato bhayaṁ”.",
  "ja432:5.1_0": "“Yena bhuttena yāpenti,",
  "ja432:5.2_0": "puthū brāhmaṇakhattiyā;",
  "ja432:5.3_0": "So maṁ bhutto byāpādeti,",
  "ja432:5.4_0": "jātaṁ saraṇato bhayaṁ”.",
  "ja432:6.1_0": "“Gimhānaṁ pacchime māse,",
  "ja432:6.2_0": "vātamicchanti paṇḍitā;",
  "ja432:6.3_0": "So maṁ bhañjati gattāni,",
  "ja432:6.4_0": "jātaṁ saraṇato bhayaṁ”.",
  "ja432:7.1_0": "“Yaṁ nissitā jagatiruhaṁ,",
  "ja432:7.2_0": "svāyaṁ aggiṁ pamuñcati;",
  "ja432:7.3_0": "Disā bhajatha vakkaṅgā,",
  "ja432:7.4_0": "jātaṁ saraṇato bhayaṁ”.",
  "ja432:8.1_0": "“Yamānayiṁ somanassaṁ,",
  "ja432:8.2_0": "māliniṁ candanussadaṁ;",
  "ja432:8.3_0": "Sā maṁ gharā nicchubhati,",
  "ja432:8.4_0": "jātaṁ saraṇato bhayaṁ”.",
  "ja432:9.1_0": "“Yena jātena nandissaṁ,",
  "ja432:9.2_0": "yassa ca bhavamicchisaṁ;",
  "ja432:9.3_0": "So maṁ gharā nicchubhati,",
  "ja432:9.4_0": "jātaṁ saraṇato bhayaṁ”.",
  "ja432:10.1_0": "“Suṇantu me jānapadā,",
  "ja432:10.2_0": "negamā ca samāgatā;",
  "ja432:10.3_0": "Yatodakaṁ tadādittaṁ,",
  "ja432:10.4_0": "yato khemaṁ tato bhayaṁ.",
  "ja432:11.1_0": "Rājā vilumpate raṭṭhaṁ,",
  "ja432:11.2_0": "brāhmaṇo ca purohito;",
  "ja432:11.3_0": "Attaguttā viharatha,",
  "ja432:11.4_0": "jātaṁ saraṇato bhayan”ti.",
  "ja432:12.1_0": "Padakusalamāṇavajātakaṁ chaṭṭhaṁ."
}