{
  "ja271:0.1_0": "Jātaka",
  "ja271:0.2_0": "Tikanipāta",
  "ja271:0.3_0": "Udapānavagga",
  "ja271:0.4_0": "1. Udapānadūsakajātaka",
  "ja271:1.1_0": "“Āraññikassa isino,",
  "ja271:1.2_0": "cirarattatapassino;",
  "ja271:1.3_0": "Kicchākataṁ udapānaṁ,",
  "ja271:1.4_0": "kathaṁ samma avāhayi”.",
  "ja271:2.1_0": "“Esa dhammo siṅgālānaṁ,",
  "ja271:2.2_0": "yaṁ pitvā ohadāmase;",
  "ja271:2.3_0": "Pitupitāmahaṁ dhammo,",
  "ja271:2.4_0": "na taṁ ujjhātumarahasi”.",
  "ja271:3.1_0": "“Yesaṁ vo ediso dhammo,",
  "ja271:3.2_0": "Adhammo pana kīdiso;",
  "ja271:3.3_0": "Mā vo dhammaṁ adhammaṁ vā,",
  "ja271:3.4_0": "Addasāma kudācanan”ti.",
  "ja271:4.1_0": "Udapānadūsakajātakaṁ paṭhamaṁ."
}