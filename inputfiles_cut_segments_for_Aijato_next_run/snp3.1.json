{
  "snp3.1:0.1_0": "Sutta Nipāta 3.1",
  "snp3.1:0.2_0": "Pabbajjāsutta",
  "snp3.1:1.1_0": "“Pabbajjaṁ kittayissāmi,",
  "snp3.1:1.2_0": "yathā pabbaji cakkhumā;",
  "snp3.1:1.3_0": "Yathā vīmaṁsamāno so,",
  "snp3.1:1.4_0": "pabbajjaṁ samarocayi.",
  "snp3.1:2.1_0": "Sambādhoyaṁ gharāvāso,",
  "snp3.1:2.2_0": "rajassāyatanaṁ iti;",
  "snp3.1:2.3_0": "Abbhokāsova pabbajjā,",
  "snp3.1:2.4_0": "iti disvāna pabbaji.",
  "snp3.1:3.1_0": "Pabbajitvāna kāyena,",
  "snp3.1:3.2_0": "pāpakammaṁ vivajjayi;",
  "snp3.1:3.3_0": "Vacīduccaritaṁ hitvā,",
  "snp3.1:3.4_0": "ājīvaṁ parisodhayi.",
  "snp3.1:4.1_0": "Agamā rājagahaṁ buddho,",
  "snp3.1:4.2_0": "magadhānaṁ giribbajaṁ;",
  "snp3.1:4.3_0": "Piṇḍāya abhihāresi,",
  "snp3.1:4.4_0": "ākiṇṇavaralakkhaṇo”.",
  "snp3.1:5.1_0": "Tamaddasā bimbisāro,",
  "snp3.1:5.2_0": "pāsādasmiṁ patiṭṭhito;",
  "snp3.1:5.3_0": "Disvā lakkhaṇasampannaṁ,",
  "snp3.1:5.4_0": "imamatthaṁ abhāsatha.",
  "snp3.1:6.1_0": "“Imaṁ bhonto nisāmetha,",
  "snp3.1:6.2_0": "abhirūpo brahā suci;",
  "snp3.1:6.3_0": "Caraṇena ca sampanno,",
  "snp3.1:6.4_0": "yugamattañca pekkhati.",
  "snp3.1:7.1_0": "Okkhittacakkhu satimā,",
  "snp3.1:7.2_0": "nāyaṁ nīcakulāmiva;",
  "snp3.1:7.3_0": "Rājadūtābhidhāvantu,",
  "snp3.1:7.4_0": "kuhiṁ bhikkhu gamissati”.",
  "snp3.1:8.1_0": "Te pesitā rājadūtā,",
  "snp3.1:8.2_0": "piṭṭhito anubandhisuṁ;",
  "snp3.1:8.3_0": "Kuhiṁ gamissati bhikkhu,",
  "snp3.1:8.4_0": "kattha vāso bhavissati.",
  "snp3.1:9.1_0": "Sapadānaṁ caramāno,",
  "snp3.1:9.2_0": "guttadvāro susaṁvuto;",
  "snp3.1:9.3_0": "Khippaṁ pattaṁ apūresi,",
  "snp3.1:9.4_0": "sampajāno paṭissato.",
  "snp3.1:10.1_0": "Piṇḍacāraṁ caritvāna,",
  "snp3.1:10.2_0": "Nikkhamma nagarā muni;",
  "snp3.1:10.3_0": "Paṇḍavaṁ abhihāresi,",
  "snp3.1:10.4_0": "Ettha vāso bhavissati.",
  "snp3.1:11.1_0": "Disvāna vāsūpagataṁ,",
  "snp3.1:11.2_0": "Tayo dūtā upāvisuṁ;",
  "snp3.1:11.3_0": "Tesu ekova āgantvā,",
  "snp3.1:11.4_0": "Rājino paṭivedayi.",
  "snp3.1:12.1_0": "“Esa bhikkhu mahārāja,",
  "snp3.1:12.2_0": "Paṇḍavassa puratthato;",
  "snp3.1:12.3_0": "Nisinno byagghusabhova,",
  "snp3.1:12.4_0": "Sīhova girigabbhare”.",
  "snp3.1:13.1_0": "Sutvāna dūtavacanaṁ,",
  "snp3.1:13.2_0": "Bhaddayānena khattiyo;",
  "snp3.1:13.3_0": "Taramānarūpo niyyāsi,",
  "snp3.1:13.4_0": "Yena paṇḍavapabbato.",
  "snp3.1:14.1_0": "Sa yānabhūmiṁ yāyitvā,",
  "snp3.1:14.2_0": "Yānā oruyha khattiyo;",
  "snp3.1:14.3_0": "Pattiko upasaṅkamma,",
  "snp3.1:14.4_0": "Āsajja naṁ upāvisi.",
  "snp3.1:15.1_0": "Nisajja rājā sammodi,",
  "snp3.1:15.2_0": "Kathaṁ sāraṇīyaṁ tato;",
  "snp3.1:15.3_0": "Kathaṁ so vītisāretvā,",
  "snp3.1:15.4_0": "Imamatthaṁ abhāsatha.",
  "snp3.1:16.1_0": "“Yuvā ca daharo cāsi,",
  "snp3.1:16.2_0": "Paṭhamuppattiko susu;",
  "snp3.1:16.3_0": "Vaṇṇārohena sampanno,",
  "snp3.1:16.4_0": "Jātimā viya khattiyo.",
  "snp3.1:17.1_0": "Sobhayanto anīkaggaṁ,",
  "snp3.1:17.2_0": "Nāgasaṅghapurakkhato;",
  "snp3.1:17.3_0": "Dadāmi bhoge bhuñjassu,",
  "snp3.1:17.4_0": "Jātiṁ akkhāhi pucchito”.",
  "snp3.1:18.1_0": "“Ujuṁ janapado rāja,",
  "snp3.1:18.2_0": "Himavantassa passato;",
  "snp3.1:18.3_0": "Dhanaviriyena sampanno,",
  "snp3.1:18.4_0": "Kosalesu niketino.",
  "snp3.1:19.1_0": "Ādiccā nāma gottena,",
  "snp3.1:19.2_0": "Sākiyā nāma jātiyā;",
  "snp3.1:19.3_0": "Tamhā kulā pabbajitomhi,",
  "snp3.1:19.4_0": "Na kāme abhipatthayaṁ.",
  "snp3.1:20.1_0": "Kāmesvādīnavaṁ disvā,",
  "snp3.1:20.2_0": "Nekkhammaṁ daṭṭhu khemato;",
  "snp3.1:20.3_0": "Padhānāya gamissāmi,",
  "snp3.1:20.4_0": "Ettha me rañjatī mano”ti.",
  "snp3.1:20.5_0": "Pabbajjāsuttaṁ paṭhamaṁ."
}