{
  "ja461:0.1_0": "Jātaka",
  "ja461:0.2_0": "Ekādasakanipāta",
  "ja461:0.3_0": "Mātuposakavagga",
  "ja461:0.4_0": "7. Dasarathajātaka",
  "ja461:1.1_0": "“Etha lakkhaṇa sītā ca,",
  "ja461:1.2_0": "ubho otarathodakaṁ;",
  "ja461:1.3_0": "Evāyaṁ bharato āha,",
  "ja461:1.4_0": "‘rājā dasaratho mato’”.",
  "ja461:2.1_0": "“Kena rāmappabhāvena,",
  "ja461:2.2_0": "socitabbaṁ na socasi;",
  "ja461:2.3_0": "Pitaraṁ kālakataṁ sutvā,",
  "ja461:2.4_0": "na taṁ pasahate dukhaṁ”.",
  "ja461:3.1_0": "“Yaṁ na sakkā nipāletuṁ,",
  "ja461:3.2_0": "posena lapataṁ bahuṁ;",
  "ja461:3.3_0": "Sa kissa viññū medhāvī,",
  "ja461:3.4_0": "attānamupatāpaye.",
  "ja461:4.1_0": "Daharā ca hi vuddhā ca,",
  "ja461:4.2_0": "ye bālā ye ca paṇḍitā;",
  "ja461:4.3_0": "Aḍḍhā ceva daliddā ca,",
  "ja461:4.4_0": "sabbe maccuparāyaṇā.",
  "ja461:5.1_0": "Phalānamiva pakkānaṁ,",
  "ja461:5.2_0": "niccaṁ patanato bhayaṁ;",
  "ja461:5.3_0": "Evaṁ jātāna maccānaṁ,",
  "ja461:5.4_0": "nicca maraṇato bhayaṁ.",
  "ja461:6.1_0": "Sāyameke na dissanti,",
  "ja461:6.2_0": "pāto diṭṭhā bahujjanā;",
  "ja461:6.3_0": "Pāto eke na dissanti,",
  "ja461:6.4_0": "sāyaṁ diṭṭhā bahujjanā.",
  "ja461:7.1_0": "Paridevayamāno ce,",
  "ja461:7.2_0": "kiñcidatthaṁ udabbahe;",
  "ja461:7.3_0": "Sammūḷho hiṁsamattānaṁ,",
  "ja461:7.4_0": "kayirā taṁ vicakkhaṇo.",
  "ja461:8.1_0": "Kiso vivaṇṇo bhavati,",
  "ja461:8.2_0": "hiṁsamattānamattano;",
  "ja461:8.3_0": "Na tena petā pālenti,",
  "ja461:8.4_0": "niratthā paridevanā.",
  "ja461:9.1_0": "Yathā saraṇamādittaṁ,",
  "ja461:9.2_0": "vārinā parinibbaye;",
  "ja461:9.3_0": "Evampi dhīro sutavā,",
  "ja461:9.4_0": "medhāvī paṇḍito naro;",
  "ja461:9.5_0": "Khippamuppatitaṁ sokaṁ,",
  "ja461:9.6_0": "vāto tūlaṁva dhaṁsaye.",
  "ja461:10.1_0": "Macco ekova acceti,",
  "ja461:10.2_0": "ekova jāyate kule;",
  "ja461:10.3_0": "Saṁyogaparamā tveva,",
  "ja461:10.4_0": "sambhogā sabbapāṇinaṁ.",
  "ja461:11.1_0": "Tasmā hi dhīrassa bahussutassa,",
  "ja461:11.2_0": "Sampassato lokamimaṁ parañca;",
  "ja461:11.3_0": "Aññāya dhammaṁ hadayaṁ manañca,",
  "ja461:11.4_0": "Sokā mahantāpi na tāpayanti.",
  "ja461:12.1_0": "Sohaṁ dassañca bhokkhañca,",
  "ja461:12.2_0": "bharissāmi ca ñātake;",
  "ja461:12.3_0": "Sesañca pālayissāmi,",
  "ja461:12.4_0": "kiccametaṁ vijānato.",
  "ja461:13.1_0": "Dasa vassasahassāni,",
  "ja461:13.2_0": "saṭṭhi vassasatāni ca;",
  "ja461:13.3_0": "Kambugīvo mahābāhu,",
  "ja461:13.4_0": "rāmo rajjamakārayī”ti.",
  "ja461:14.1_0": "Dasarathajātakaṁ sattamaṁ."
}