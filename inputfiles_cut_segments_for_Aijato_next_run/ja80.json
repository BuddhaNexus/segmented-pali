{
  "ja80:0.1_0": "Jātaka",
  "ja80:0.2_0": "Ekakanipāta",
  "ja80:0.3_0": "Varuṇavagga",
  "ja80:0.4_0": "10. Bhīmasenajātaka",
  "ja80:1.1_0": "“Yaṁ te pavikatthitaṁ pure,",
  "ja80:1.2_0": "Atha te pūtisarā sajanti pacchā;",
  "ja80:1.3_0": "Ubhayaṁ na sameti bhīmasena,",
  "ja80:1.4_0": "Yuddhakathā ca idañca te vihaññan”ti.",
  "ja80:2.1_0": "Bhīmasenajātakaṁ dasamaṁ.",
  "ja80:2.2_0": "Varuṇavaggo aṭṭhamo.",
  "ja80:3.1_0": "Tassuddānaṁ",
  "ja80:4.1_0": "Varuṇā akataññūvare tu saccavaraṁ,",
  "ja80:4.2_0": "Savanappatinā ca abhitthanaya;",
  "ja80:4.3_0": "Karuṇāya silāplava illisato,",
  "ja80:4.4_0": "Puna ḍiṇḍimapūtisarena dasāti."
}