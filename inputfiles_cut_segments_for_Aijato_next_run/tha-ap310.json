{
  "tha-ap310:0.1_0": "Therāpadāna",
  "tha-ap310:0.2_0": "Padumakesaravagga",
  "tha-ap310:0.3_0": "8. Anulepadāyakattheraapadāna",
  "tha-ap310:1.1_0": "“Atthadassissa munino,",
  "tha-ap310:1.2_0": "addasaṁ sāvakaṁ ahaṁ;",
  "tha-ap310:1.3_0": "Navakammaṁ karontassa,",
  "tha-ap310:1.4_0": "sīmāya upagacchahaṁ.",
  "tha-ap310:2.1_0": "Niṭṭhite navakamme ca,",
  "tha-ap310:2.2_0": "anulepamadāsahaṁ;",
  "tha-ap310:2.3_0": "Pasannacitto sumano,",
  "tha-ap310:2.4_0": "puññakkhette anuttare.",
  "tha-ap310:3.1_0": "Aṭṭhārase kappasate,",
  "tha-ap310:3.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap310:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap310:3.4_0": "anulepassidaṁ phalaṁ.",
  "tha-ap310:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap310:4.2_0": "…pe…",
  "tha-ap310:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap310:5.1_0": "Itthaṁ sudaṁ āyasmā anulepadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap310:6.1_0": "Anulepadāyakattherassāpadānaṁ aṭṭhamaṁ."
}