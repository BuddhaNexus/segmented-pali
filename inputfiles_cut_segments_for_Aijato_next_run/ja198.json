{
  "ja198:0.1_0": "Jātaka",
  "ja198:0.2_0": "Dukanipāta",
  "ja198:0.3_0": "Ruhakavagga",
  "ja198:0.4_0": "8. Rādhajātaka",
  "ja198:1.1_0": "“Pavāsā āgato tāta,",
  "ja198:1.2_0": "idāni nacirāgato;",
  "ja198:1.3_0": "Kaccinnu tāta te mātā,",
  "ja198:1.4_0": "na aññamupasevati”.",
  "ja198:2.1_0": "“Na kho panetaṁ subhaṇaṁ,",
  "ja198:2.2_0": "Giraṁ saccupasaṁhitaṁ;",
  "ja198:2.3_0": "Sayetha poṭṭhapādova,",
  "ja198:2.4_0": "Mummure upakūthito”ti.",
  "ja198:3.1_0": "Rādhajātakaṁ aṭṭhamaṁ."
}