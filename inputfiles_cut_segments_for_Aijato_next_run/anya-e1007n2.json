{
  "anya-e1007n2:0_0": "Cāṇakyanītilā gāthā",
  "anya-e1007n2:1_0": "Lālane bahavo dosā,",
  "anya-e1007n2:2_0": "Tāḷane bahavo guṇā;",
  "anya-e1007n2:3_0": "Tasmā puttañca sissañca,",
  "anya-e1007n2:4_0": "Tāḷaye na ca lālaye.",
  "anya-e1007n2:5_0": "28.",
  "anya-e1007n2:6_0": "Atītassa hi mittassa,",
  "anya-e1007n2:7_0": "Yo ce dosaṁ pakāsaye;",
  "anya-e1007n2:8_0": "So have paccuppannassa,",
  "anya-e1007n2:9_0": "Dosaṁ bhāseti ñāyati.",
  "anya-e1007n2:10_0": "29.",
  "anya-e1007n2:11_0": "Latāviya sevakā te,",
  "anya-e1007n2:12_0": "Ye nissayaṁ palambare;",
  "anya-e1007n2:13_0": "Nissayassa vināsena,",
  "anya-e1007n2:14_0": "Bhūmyaṁ senti anāthakā.",
  "anya-e1007n2:15_0": "30.",
  "anya-e1007n2:16_0": "Dosasiṅgehi vijjhanto,",
  "anya-e1007n2:17_0": "Mānakhūrehi akkamaṁ;",
  "anya-e1007n2:18_0": "Bhayaṁ karoti lokamhi,",
  "anya-e1007n2:19_0": "Gova bālo vihiṁsako.",
  "anya-e1007n2:20_0": "31.",
  "anya-e1007n2:21_0": "Ādo upari lokoyaṁ,",
  "anya-e1007n2:22_0": "Ujulekhāya tiṭṭhati;",
  "anya-e1007n2:23_0": "Musāvātehi taṁlokaṁ,",
  "anya-e1007n2:24_0": "Nipātesi anajjavaṁ.",
  "anya-e1007n2:25_0": "32.",
  "anya-e1007n2:26_0": "Sughaṭaṁ kumbhakārena,",
  "anya-e1007n2:27_0": "Nāraho paribhuñjituṁ;",
  "anya-e1007n2:28_0": "Tathūpamāya vekkheyya,",
  "anya-e1007n2:29_0": "Sakammaparakammani.",
  "anya-e1007n2:30_0": "33.",
  "anya-e1007n2:31_0": "Anantaraṁsī sūropi,",
  "anya-e1007n2:32_0": "Nasakkoti ghanaṁ tamaṁ;",
  "anya-e1007n2:33_0": "Vijjhituṁ raṁsiyā loke,",
  "anya-e1007n2:34_0": "Tathā madanamohitā;",
  "anya-e1007n2:35_0": "Nasakkonti madaṁ bhetvā,",
  "anya-e1007n2:36_0": "Paññābhāya pabhāsituṁ.",
  "anya-e1007n2:37_0": "34.",
  "anya-e1007n2:38_0": "Khedaveraṁ daliddamhi,",
  "anya-e1007n2:39_0": "Bhogimhi rogupaddavaṁ;",
  "anya-e1007n2:40_0": "Dessaverañca āṇimhi,",
  "anya-e1007n2:41_0": "Passe lokassa veritaṁ.",
  "anya-e1007n2:42_0": "35.",
  "anya-e1007n2:43_0": "Saṁladdhena subhogena,",
  "anya-e1007n2:44_0": "Jīvaṁ suddhaṁ kare nijaṁ;",
  "anya-e1007n2:45_0": "Seṭṭho so tena jīvena,",
  "anya-e1007n2:46_0": "Jeguccho malajīviko.",
  "anya-e1007n2:47_0": "36.",
  "anya-e1007n2:48_0": "Vajira puppharāgānaṁ,",
  "anya-e1007n2:49_0": "Visesaṁ yo nabujjhati;",
  "anya-e1007n2:50_0": "Kathañhi so vikkīṇeyya,",
  "anya-e1007n2:51_0": "Kīṇeyya vā yathātathaṁ.",
  "anya-e1007n2:52_0": "37.",
  "anya-e1007n2:53_0": "Kippīli kopi cintetvā,",
  "anya-e1007n2:54_0": "Pabbataṁ bhettu mussahaṁ;",
  "anya-e1007n2:55_0": "Abalā tanumajjhattā,",
  "anya-e1007n2:56_0": "Cintā hasyāva sā mudhā.",
  "anya-e1007n2:57_0": "38.",
  "anya-e1007n2:58_0": "Jātamattaṁ na yo sattuṁ,",
  "anya-e1007n2:59_0": "Rogañcūpasamaṁ naye;",
  "anya-e1007n2:60_0": "Mahābalopi teneva,",
  "anya-e1007n2:61_0": "Vuddhiṁpatvā sa haññate.",
  "anya-e1007n2:62_0": "39.",
  "anya-e1007n2:63_0": "Sajīvamaṁsabhakkhehi,",
  "anya-e1007n2:64_0": "Sadāṭhīhi mukhehi bho;",
  "anya-e1007n2:65_0": "Biḷārabyagghasīhānaṁ,",
  "anya-e1007n2:66_0": "Nihīnāni anekadhā;",
  "anya-e1007n2:67_0": "Tikkhāni kharavādāni,",
  "anya-e1007n2:68_0": "Manussānaṁ mukhāni ve.",
  "anya-e1007n2:69_0": "40.",
  "anya-e1007n2:70_0": "Viluppantā vidhāvanti,",
  "anya-e1007n2:71_0": "Sajīvavuttikammunā;",
  "anya-e1007n2:72_0": "Janā tena vihaññanti,",
  "anya-e1007n2:73_0": "Caranti dhammavemukhā.",
  "anya-e1007n2:74_0": "41.",
  "anya-e1007n2:75_0": "Sulabhaṁ lokiyaṁ loke,",
  "anya-e1007n2:76_0": "Sāsanīyaṁva dullabhaṁ;",
  "anya-e1007n2:77_0": "Dullabhaṁ taṁ vamaññanto,",
  "anya-e1007n2:78_0": "Eso bālatamo bhave.",
  "anya-e1007n2:79_0": "42.",
  "anya-e1007n2:80_0": "Yo patittha agyāvāṭaṁ,",
  "anya-e1007n2:81_0": "Mohā taṁ upakārituṁ;",
  "anya-e1007n2:82_0": "Aññorohi tadā vāṭaṁ,",
  "anya-e1007n2:83_0": "Dutīyo muḷhamuḷhako.",
  "anya-e1007n2:84_0": "43.",
  "anya-e1007n2:85_0": "Byaggho āvudhaviddho hi,",
  "anya-e1007n2:86_0": "Akā duṭṭhāni ninnadaṁ;",
  "anya-e1007n2:87_0": "Tatheva sādhusatthena,",
  "anya-e1007n2:88_0": "Viddho bālo pakuppito.",
  "anya-e1007n2:89_0": "44.",
  "anya-e1007n2:90_0": "Pivanti lohitaṁ ḍaṁsā,",
  "anya-e1007n2:91_0": "Anto tuṇḍena makkhikā;",
  "anya-e1007n2:92_0": "Bahiddhā parivārenti,",
  "anya-e1007n2:93_0": "Jano tena ḍaṁsāyaye.",
  "anya-e1007n2:94_0": "45.",
  "anya-e1007n2:95_0": "Adhanassa khaṇo appo,",
  "anya-e1007n2:96_0": "Saddhammo appakālino;",
  "anya-e1007n2:97_0": "Appako tena yuñjeyyuṁ,",
  "anya-e1007n2:98_0": "Khaṇaṁ bahuṁ labhetave."
}