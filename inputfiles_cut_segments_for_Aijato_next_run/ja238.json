{
  "ja238:0.1_0": "Jātaka",
  "ja238:0.2_0": "Dukanipāta",
  "ja238:0.3_0": "Upāhanavagga",
  "ja238:0.4_0": "8. Ekapadajātaka",
  "ja238:1.1_0": "“Iṅgha ekapadaṁ tāta,",
  "ja238:1.2_0": "anekatthapadassitaṁ;",
  "ja238:1.3_0": "Kiñci saṅgāhikaṁ brūsi,",
  "ja238:1.4_0": "yenatthe sādhayemase”.",
  "ja238:2.1_0": "“Dakkheyyekapadaṁ tāta,",
  "ja238:2.2_0": "anekatthapadassitaṁ;",
  "ja238:2.3_0": "Tañca sīlena saññuttaṁ,",
  "ja238:2.4_0": "khantiyā upapāditaṁ;",
  "ja238:2.5_0": "Alaṁ mitte sukhāpetuṁ,",
  "ja238:2.6_0": "amittānaṁ dukhāya cā”ti.",
  "ja238:3.1_0": "Ekapadajātakaṁ aṭṭhamaṁ."
}