{
  "ja257:0.1_0": "Jātaka",
  "ja257:0.2_0": "Tikanipāta",
  "ja257:0.3_0": "Saṅkappavagga",
  "ja257:0.4_0": "7. Gāmaṇicandajātaka",
  "ja257:1.1_0": "“Nāyaṁ gharānaṁ kusalo,",
  "ja257:1.2_0": "lolo ayaṁ valīmukho;",
  "ja257:1.3_0": "Kataṁ kataṁ kho dūseyya,",
  "ja257:1.4_0": "evaṁdhammamidaṁ kulaṁ.",
  "ja257:2.1_0": "Nayidaṁ cittavato lomaṁ,",
  "ja257:2.2_0": "nāyaṁ assāsiko migo;",
  "ja257:2.3_0": "Siṭṭhaṁ me janasandhena,",
  "ja257:2.4_0": "nāyaṁ kiñci vijānati.",
  "ja257:3.1_0": "Na mātaraṁ pitaraṁ vā,",
  "ja257:3.2_0": "bhātaraṁ bhaginiṁ sakaṁ;",
  "ja257:3.3_0": "Bhareyya tādiso poso,",
  "ja257:3.4_0": "siṭṭhaṁ dasarathena me”ti.",
  "ja257:4.1_0": "Gāmaṇicanda jātakaṁ sattamaṁ."
}