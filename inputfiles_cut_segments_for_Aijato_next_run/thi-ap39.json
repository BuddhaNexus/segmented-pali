{
  "thi-ap39:0.1_0": "Therīapadāna",
  "thi-ap39:0.2_0": "Khattiyāvagga",
  "thi-ap39:0.3_0": "9 Ambapālitherīapadāna",
  "thi-ap39:1.1_0": "“Yo raṁsiphusitāveḷo,",
  "thi-ap39:1.2_0": "phusso nāma mahāmuni;",
  "thi-ap39:1.3_0": "Tassāhaṁ bhaginī āsiṁ,",
  "thi-ap39:1.4_0": "ajāyiṁ khattiye kule.",
  "thi-ap39:2.1_0": "Tassa dhammaṁ suṇitvāhaṁ,",
  "thi-ap39:2.2_0": "vippasannena cetasā;",
  "thi-ap39:2.3_0": "Mahādānaṁ daditvāna,",
  "thi-ap39:2.4_0": "patthayiṁ rūpasampadaṁ.",
  "thi-ap39:3.1_0": "Ekattiṁse ito kappe,",
  "thi-ap39:3.2_0": "sikhī lokagganāyako;",
  "thi-ap39:3.3_0": "Uppanno lokapajjoto,",
  "thi-ap39:3.4_0": "tilokasaraṇo jino.",
  "thi-ap39:4.1_0": "Tadāruṇapure ramme,",
  "thi-ap39:4.2_0": "brāhmaññakulasambhavā;",
  "thi-ap39:4.3_0": "Vimuttacittaṁ kupitā,",
  "thi-ap39:4.4_0": "bhikkhuniṁ abhisāpayiṁ.",
  "thi-ap39:5.1_0": "‘Vesikāva anācārā’,",
  "thi-ap39:5.2_0": "jinasāsanadūsikā;",
  "thi-ap39:5.3_0": "Evaṁ akkosayitvāna,",
  "thi-ap39:5.4_0": "tena pāpena kammunā.",
  "thi-ap39:6.1_0": "Dāruṇaṁ nirayaṁ gantvā,",
  "thi-ap39:6.2_0": "mahādukkhasamappitā;",
  "thi-ap39:6.3_0": "Tato cutā manussesu,",
  "thi-ap39:6.4_0": "upapannā tapassinī.",
  "thi-ap39:7.1_0": "Dasajātisahassāni,",
  "thi-ap39:7.2_0": "gaṇikattamakārayiṁ;",
  "thi-ap39:7.3_0": "Tamhā pāpā na muccissaṁ,",
  "thi-ap39:7.4_0": "bhutvā duṭṭhavisaṁ yathā.",
  "thi-ap39:8.1_0": "Brahmacariyamasevissaṁ,",
  "thi-ap39:8.2_0": "kassape jinasāsane;",
  "thi-ap39:8.3_0": "Tena kammavipākena,",
  "thi-ap39:8.4_0": "ajāyiṁ tidase pure.",
  "thi-ap39:9.1_0": "Pacchime bhave sampatte,",
  "thi-ap39:9.2_0": "ahosiṁ opapātikā;",
  "thi-ap39:9.3_0": "Ambasākhantare jātā,",
  "thi-ap39:9.4_0": "ambapālīti tenahaṁ.",
  "thi-ap39:10.1_0": "Parivutā pāṇakoṭīhi,",
  "thi-ap39:10.2_0": "pabbajiṁ jinasāsane;",
  "thi-ap39:10.3_0": "Pattāhaṁ acalaṁ ṭhānaṁ,",
  "thi-ap39:10.4_0": "dhītā buddhassa orasā.",
  "thi-ap39:11.1_0": "Iddhīsu ca vasī homi,",
  "thi-ap39:11.2_0": "sotadhātuvisuddhiyā;",
  "thi-ap39:11.3_0": "Cetopariyañāṇassa,",
  "thi-ap39:11.4_0": "vasī homi mahāmuni.",
  "thi-ap39:12.1_0": "Pubbenivāsaṁ jānāmi,",
  "thi-ap39:12.2_0": "dibbacakkhu visodhitaṁ;",
  "thi-ap39:12.3_0": "Sabbāsavā parikkhīṇā,",
  "thi-ap39:12.4_0": "natthi dāni punabbhavo.",
  "thi-ap39:13.1_0": "Atthadhammaniruttīsu,",
  "thi-ap39:13.2_0": "paṭibhāne tatheva ca;",
  "thi-ap39:13.3_0": "Ñāṇaṁ me vimalaṁ suddhaṁ,",
  "thi-ap39:13.4_0": "buddhaseṭṭhassa vāhasā.",
  "thi-ap39:14.1_0": "Kilesā jhāpitā mayhaṁ,",
  "thi-ap39:14.2_0": "…pe…",
  "thi-ap39:14.3_0": "viharāmi anāsavā.",
  "thi-ap39:15.1_0": "Svāgataṁ vata me āsi,",
  "thi-ap39:15.2_0": "…pe…",
  "thi-ap39:15.3_0": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap39:16.1_0": "Paṭisambhidā catasso,",
  "thi-ap39:16.2_0": "…pe…",
  "thi-ap39:16.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "thi-ap39:17.1_0": "Itthaṁ sudaṁ ambapāli bhikkhunī imā gāthāyo abhāsitthāti.",
  "thi-ap39:18.1_0": "Ambapālitheriyāpadānaṁ navamaṁ."
}