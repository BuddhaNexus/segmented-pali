{
  "tha-ap222:0.1_0": "Therāpadāna",
  "tha-ap222:0.2_0": "Hatthivagga",
  "tha-ap222:0.3_0": "10. Pāpanivāriyattheraapadāna",
  "tha-ap222:1.1_0": "“Piyadassissa bhagavato,",
  "tha-ap222:1.2_0": "caṅkamaṁ sodhitaṁ mayā;",
  "tha-ap222:1.3_0": "Naḷakehi paṭicchannaṁ,",
  "tha-ap222:1.4_0": "vātātapanivāraṇaṁ.",
  "tha-ap222:2.1_0": "Pāpaṁ vivajjanatthāya,",
  "tha-ap222:2.2_0": "kusalassupasampadā;",
  "tha-ap222:2.3_0": "Kilesānaṁ pahānāya,",
  "tha-ap222:2.4_0": "padahiṁ satthu sāsane.",
  "tha-ap222:3.1_0": "Ito ekādase kappe,",
  "tha-ap222:3.2_0": "aggidevoti vissuto;",
  "tha-ap222:3.3_0": "Sattaratanasampanno,",
  "tha-ap222:3.4_0": "cakkavattī mahabbalo.",
  "tha-ap222:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap222:4.2_0": "…pe…",
  "tha-ap222:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap222:5.1_0": "Itthaṁ sudaṁ āyasmā pāpanivāriyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap222:6.1_0": "Pāpanivāriyattherassāpadānaṁ dasamaṁ.",
  "tha-ap222:7.1_0": "Hatthivaggo bāvīsatimo.",
  "tha-ap222:8.1_0": "Tassuddānaṁ",
  "tha-ap222:9.1_0": "Hatthi pānadhi saccañca,",
  "tha-ap222:9.2_0": "ekasaññi ca raṁsiko;",
  "tha-ap222:9.3_0": "Sandhito tālavaṇṭañca,",
  "tha-ap222:9.4_0": "tathā akkantasaññako;",
  "tha-ap222:9.5_0": "Sappi pāpanivārī ca,",
  "tha-ap222:9.6_0": "catuppaññāsa gāthakāti."
}