{
  "kv21.5:0.1_0": "Kathāvatthu",
  "kv21.5:0.2_0": "Mahāpaṇṇāsaka",
  "kv21.5:0.3_0": "Ekavīsatimavagga",
  "kv21.5:0.4_0": "Buddhakathā",
  "kv21.5:1.1_0": "Atthi buddhānaṁ buddhehi hīnātirekatāti?",
  "kv21.5:1.2_0": "Āmantā.",
  "kv21.5:1.3_0": "Satipaṭṭhānatoti?",
  "kv21.5:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv21.5:1.5_0": "sammappadhānato …pe…",
  "kv21.5:1.6_0": "iddhipādato …",
  "kv21.5:1.7_0": "indriyato …",
  "kv21.5:1.8_0": "balato …",
  "kv21.5:1.9_0": "bojjhaṅgato …",
  "kv21.5:1.10_0": "vasibhāvato …pe…",
  "kv21.5:1.11_0": "sabbaññutañāṇadassanatoti?",
  "kv21.5:1.12_0": "Na hevaṁ vattabbe …pe….",
  "kv21.5:1.13_0": "Buddhakathā niṭṭhitā."
}