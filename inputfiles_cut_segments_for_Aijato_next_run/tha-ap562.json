{
  "tha-ap562:0.1_0": "Therāpadāna",
  "tha-ap562:0.2_0": "Yasavagga",
  "tha-ap562:0.3_0": "10. Upavānattheraapadāna",
  "tha-ap562:1.1_0": "“Padumuttaro nāma jino,",
  "tha-ap562:1.2_0": "sabbadhammāna pāragū;",
  "tha-ap562:1.3_0": "Jalitvā aggikkhandhova,",
  "tha-ap562:1.4_0": "sambuddho parinibbuto.",
  "tha-ap562:2.1_0": "Mahājanā samāgamma,",
  "tha-ap562:2.2_0": "pūjayitvā tathāgataṁ;",
  "tha-ap562:2.3_0": "Citakaṁ katvā sugataṁ,",
  "tha-ap562:2.4_0": "sarīraṁ abhiropayuṁ.",
  "tha-ap562:3.1_0": "Sarīrakiccaṁ katvāna,",
  "tha-ap562:3.2_0": "dhātuṁ tattha samānayuṁ;",
  "tha-ap562:3.3_0": "Sadevamanussā sabbe,",
  "tha-ap562:3.4_0": "buddhathūpaṁ akaṁsu te.",
  "tha-ap562:4.1_0": "Paṭhamā kañcanamayā,",
  "tha-ap562:4.2_0": "dutiyā ca maṇimayā;",
  "tha-ap562:4.3_0": "Tatiyā rūpiyamayā,",
  "tha-ap562:4.4_0": "catutthī phalikāmayā.",
  "tha-ap562:5.1_0": "Tattha pañcamikā ceva,",
  "tha-ap562:5.2_0": "lohitaṅkamayā ahu;",
  "tha-ap562:5.3_0": "Chaṭṭhā masāragallassa,",
  "tha-ap562:5.4_0": "sabbaṁ ratanamayūpari.",
  "tha-ap562:6.1_0": "Jaṅghā maṇimayā āsi,",
  "tha-ap562:6.2_0": "vedikā ratanāmayā;",
  "tha-ap562:6.3_0": "Sabbasoṇṇamayo thūpo,",
  "tha-ap562:6.4_0": "uddhaṁ yojanamuggato.",
  "tha-ap562:7.1_0": "Devā tattha samāgantvā,",
  "tha-ap562:7.2_0": "ekato mantayuṁ tadā;",
  "tha-ap562:7.3_0": "‘Mayampi thūpaṁ kassāma,",
  "tha-ap562:7.4_0": "lokanāthassa tādino.",
  "tha-ap562:8.1_0": "Dhātu āveṇikā natthi,",
  "tha-ap562:8.2_0": "sarīraṁ ekapiṇḍitaṁ;",
  "tha-ap562:8.3_0": "Imamhi buddhathūpamhi,",
  "tha-ap562:8.4_0": "kassāma kañcukaṁ mayaṁ’.",
  "tha-ap562:9.1_0": "Devā sattahi ratnehi,",
  "tha-ap562:9.2_0": "aññaṁ vaḍḍhesuṁ yojanaṁ;",
  "tha-ap562:9.3_0": "Thūpo dviyojanubbedho,",
  "tha-ap562:9.4_0": "timiraṁ byapahanti so.",
  "tha-ap562:10.1_0": "Nāgā tattha samāgantvā,",
  "tha-ap562:10.2_0": "ekato mantayuṁ tadā;",
  "tha-ap562:10.3_0": "‘Manussā ceva devā ca,",
  "tha-ap562:10.4_0": "buddhathūpaṁ akaṁsu te.",
  "tha-ap562:11.1_0": "Mā no pamattā assumha,",
  "tha-ap562:11.2_0": "appamattā sadevakā;",
  "tha-ap562:11.3_0": "Mayampi thūpaṁ kassāma,",
  "tha-ap562:11.4_0": "lokanāthassa tādino’.",
  "tha-ap562:12.1_0": "Indanīlaṁ mahānīlaṁ,",
  "tha-ap562:12.2_0": "atho jotirasaṁ maṇiṁ;",
  "tha-ap562:12.3_0": "Ekato sannipātetvā,",
  "tha-ap562:12.4_0": "buddhathūpaṁ achādayuṁ.",
  "tha-ap562:13.1_0": "Sabbaṁ maṇimayaṁ āsi,",
  "tha-ap562:13.2_0": "yāvatā buddhacetiyaṁ;",
  "tha-ap562:13.3_0": "Tiyojanasamubbedhaṁ,",
  "tha-ap562:13.4_0": "ālokakaraṇaṁ tadā.",
  "tha-ap562:14.1_0": "Garuḷā ca samāgantvā,",
  "tha-ap562:14.2_0": "ekato mantayuṁ tadā;",
  "tha-ap562:14.3_0": "‘Manussā devanāgā ca,",
  "tha-ap562:14.4_0": "buddhapūjaṁ akaṁsu te.",
  "tha-ap562:15.1_0": "Mā no pamattā assumha,",
  "tha-ap562:15.2_0": "appamattā sadevakā;",
  "tha-ap562:15.3_0": "Mayampi thūpaṁ kassāma,",
  "tha-ap562:15.4_0": "lokanāthassa tādino’.",
  "tha-ap562:16.1_0": "Sabbaṁ maṇimayaṁ thūpaṁ,",
  "tha-ap562:16.2_0": "akaruṁ te ca kañcukaṁ;",
  "tha-ap562:16.3_0": "Yojanaṁ tepi vaḍḍhesuṁ,",
  "tha-ap562:16.4_0": "āyataṁ buddhacetiyaṁ.",
  "tha-ap562:17.1_0": "Catuyojanamubbedho,",
  "tha-ap562:17.2_0": "buddhathūpo virocati;",
  "tha-ap562:17.3_0": "Obhāseti disā sabbā,",
  "tha-ap562:17.4_0": "sataraṁsīva uggato.",
  "tha-ap562:18.1_0": "Kumbhaṇḍā ca samāgantvā,",
  "tha-ap562:18.2_0": "ekato mantayuṁ tadā;",
  "tha-ap562:18.3_0": "‘Manussā ceva devā ca,",
  "tha-ap562:18.4_0": "nāgā ca garuḷā tathā.",
  "tha-ap562:19.1_0": "Paccekaṁ buddhaseṭṭhassa,",
  "tha-ap562:19.2_0": "akaṁsu thūpamuttamaṁ;",
  "tha-ap562:19.3_0": "Mā no pamattā assumha,",
  "tha-ap562:19.4_0": "appamattā sadevakā.",
  "tha-ap562:20.1_0": "Mayampi thūpaṁ kassāma,",
  "tha-ap562:20.2_0": "lokanāthassa tādino;",
  "tha-ap562:20.3_0": "Ratanehi chādessāma,",
  "tha-ap562:20.4_0": "āyataṁ buddhacetiyaṁ’.",
  "tha-ap562:21.1_0": "Yojanaṁ tepi vaḍḍhesuṁ,",
  "tha-ap562:21.2_0": "āyataṁ buddhacetiyaṁ;",
  "tha-ap562:21.3_0": "Pañcayojanamubbedho,",
  "tha-ap562:21.4_0": "thūpo obhāsate tadā.",
  "tha-ap562:22.1_0": "Yakkhā tattha samāgantvā,",
  "tha-ap562:22.2_0": "ekato mantayuṁ tadā;",
  "tha-ap562:22.3_0": "‘Manussā devanāgā ca,",
  "tha-ap562:22.4_0": "garuḷā ca kumbhaṇḍakā.",
  "tha-ap562:23.1_0": "Paccekaṁ buddhaseṭṭhassa,",
  "tha-ap562:23.2_0": "akaṁsu thūpamuttamaṁ;",
  "tha-ap562:23.3_0": "Mā no pamattā assumha,",
  "tha-ap562:23.4_0": "appamattā sadevakā.",
  "tha-ap562:24.1_0": "Mayampi thūpaṁ kassāma,",
  "tha-ap562:24.2_0": "lokanāthassa tādino;",
  "tha-ap562:24.3_0": "Phalikā chādayissāma,",
  "tha-ap562:24.4_0": "āyataṁ buddhacetiyaṁ’.",
  "tha-ap562:25.1_0": "Yojanaṁ tepi vaḍḍhesuṁ,",
  "tha-ap562:25.2_0": "āyataṁ buddhacetiyaṁ;",
  "tha-ap562:25.3_0": "Chayojanikamubbedho,",
  "tha-ap562:25.4_0": "thūpo obhāsate tadā.",
  "tha-ap562:26.1_0": "Gandhabbā ca samāgantvā,",
  "tha-ap562:26.2_0": "ekato mantayuṁ tadā;",
  "tha-ap562:26.3_0": "‘Manujā devatā nāgā,",
  "tha-ap562:26.4_0": "kumbhaṇḍā guyhakā tathā.",
  "tha-ap562:27.1_0": "Sabbe akaṁsu buddhathūpaṁ,",
  "tha-ap562:27.2_0": "mayamettha akārakā;",
  "tha-ap562:27.3_0": "Mayampi thūpaṁ kassāma,",
  "tha-ap562:27.4_0": "lokanāthassa tādino’.",
  "tha-ap562:28.1_0": "Vediyo satta katvāna,",
  "tha-ap562:28.2_0": "dhajaṁ chattaṁ akaṁsu te;",
  "tha-ap562:28.3_0": "Sabbasoṇṇamayaṁ thūpaṁ,",
  "tha-ap562:28.4_0": "gandhabbā kārayuṁ tadā.",
  "tha-ap562:29.1_0": "Sattayojanamubbedho,",
  "tha-ap562:29.2_0": "thūpo obhāsate tadā;",
  "tha-ap562:29.3_0": "Rattindivā na ñāyanti,",
  "tha-ap562:29.4_0": "āloko hoti sabbadā.",
  "tha-ap562:30.1_0": "Abhibhonti na tassābhā,",
  "tha-ap562:30.2_0": "candasūrā satārakā;",
  "tha-ap562:30.3_0": "Samantā yojanasate,",
  "tha-ap562:30.4_0": "padīpopi na pajjali.",
  "tha-ap562:31.1_0": "Tena kālena ye keci,",
  "tha-ap562:31.2_0": "thūpaṁ pūjenti mānusā;",
  "tha-ap562:31.3_0": "Na te thūpaṁ āruhanti,",
  "tha-ap562:31.4_0": "ambare ukkhipanti te.",
  "tha-ap562:32.1_0": "Devehi ṭhapito yakkho,",
  "tha-ap562:32.2_0": "abhisammatanāmako;",
  "tha-ap562:32.3_0": "Dhajaṁ vā pupphadāmaṁ vā,",
  "tha-ap562:32.4_0": "abhiropeti uttariṁ.",
  "tha-ap562:33.1_0": "Na te passanti taṁ yakkhaṁ,",
  "tha-ap562:33.2_0": "dāmaṁ passanti gacchato;",
  "tha-ap562:33.3_0": "Evaṁ passitvā gacchantā,",
  "tha-ap562:33.4_0": "sabbe gacchanti suggatiṁ.",
  "tha-ap562:34.1_0": "Viruddhā ye pāvacane,",
  "tha-ap562:34.2_0": "pasannā ye ca sāsane;",
  "tha-ap562:34.3_0": "Pāṭihīraṁ daṭṭhukāmā,",
  "tha-ap562:34.4_0": "thūpaṁ pūjenti mānusā.",
  "tha-ap562:35.1_0": "Nagare haṁsavatiyā,",
  "tha-ap562:35.2_0": "ahosiṁ bhatako tadā;",
  "tha-ap562:35.3_0": "Āmoditaṁ janaṁ disvā,",
  "tha-ap562:35.4_0": "evaṁ cintesahaṁ tadā.",
  "tha-ap562:36.1_0": "‘Uḷāro bhagavā neso,",
  "tha-ap562:36.2_0": "yassa dhātughare disaṁ;",
  "tha-ap562:36.3_0": "Imā ca janatā tuṭṭhā,",
  "tha-ap562:36.4_0": "kāraṁ kubbaṁ na tappare.",
  "tha-ap562:37.1_0": "Ahampi kāraṁ kassāmi,",
  "tha-ap562:37.2_0": "lokanāthassa tādino;",
  "tha-ap562:37.3_0": "Tassa dhammesu dāyādo,",
  "tha-ap562:37.4_0": "bhavissāmi anāgate’.",
  "tha-ap562:38.1_0": "Sudhotaṁ rajakenāhaṁ,",
  "tha-ap562:38.2_0": "uttareyyaṁ paṭaṁ mama;",
  "tha-ap562:38.3_0": "Veḷagge ālaggetvāna,",
  "tha-ap562:38.4_0": "dhajaṁ ukkhipimambare.",
  "tha-ap562:39.1_0": "Abhisammatako gayha,",
  "tha-ap562:39.2_0": "ambare hāsi me dhajaṁ;",
  "tha-ap562:39.3_0": "Vāteritaṁ dhajaṁ disvā,",
  "tha-ap562:39.4_0": "bhiyyo hāsaṁ janesahaṁ.",
  "tha-ap562:40.1_0": "Tattha cittaṁ pasādetvā,",
  "tha-ap562:40.2_0": "samaṇaṁ upasaṅkamiṁ;",
  "tha-ap562:40.3_0": "Taṁ bhikkhuṁ abhivādetvā,",
  "tha-ap562:40.4_0": "vipākaṁ pucchahaṁ dhaje.",
  "tha-ap562:41.1_0": "So me kathesi ānandī,",
  "tha-ap562:41.2_0": "pītisañjananaṁ mama;",
  "tha-ap562:41.3_0": "‘Tassa dhajassa vipākaṁ,",
  "tha-ap562:41.4_0": "anubhossasi sabbadā.",
  "tha-ap562:42.1_0": "Hatthiassarathāpattī,",
  "tha-ap562:42.2_0": "senā ca caturaṅginī;",
  "tha-ap562:42.3_0": "Parivāressanti taṁ niccaṁ,",
  "tha-ap562:42.4_0": "dhajadānassidaṁ phalaṁ.",
  "tha-ap562:43.1_0": "Saṭṭhituriyasahassāni,",
  "tha-ap562:43.2_0": "bheriyo samalaṅkatā;",
  "tha-ap562:43.3_0": "Parivāressanti taṁ niccaṁ,",
  "tha-ap562:43.4_0": "dhajadānassidaṁ phalaṁ.",
  "tha-ap562:44.1_0": "Chaḷasītisahassāni,",
  "tha-ap562:44.2_0": "nāriyo samalaṅkatā;",
  "tha-ap562:44.3_0": "Vicittavatthābharaṇā,",
  "tha-ap562:44.4_0": "āmukkamaṇikuṇḍalā.",
  "tha-ap562:45.1_0": "Aḷārapamhā hasulā,",
  "tha-ap562:45.2_0": "susaññā tanumajjhimā;",
  "tha-ap562:45.3_0": "Parivāressanti taṁ niccaṁ,",
  "tha-ap562:45.4_0": "dhajadānassidaṁ phalaṁ.",
  "tha-ap562:46.1_0": "Tiṁsakappasahassāni,",
  "tha-ap562:46.2_0": "devaloke ramissasi;",
  "tha-ap562:46.3_0": "Asītikkhattuṁ devindo,",
  "tha-ap562:46.4_0": "devarajjaṁ karissasi.",
  "tha-ap562:47.1_0": "Sahassakkhattuṁ rājā ca,",
  "tha-ap562:47.2_0": "cakkavattī bhavissati;",
  "tha-ap562:47.3_0": "Padesarajjaṁ vipulaṁ,",
  "tha-ap562:47.4_0": "gaṇanāto asaṅkhiyaṁ.",
  "tha-ap562:48.1_0": "Kappasatasahassamhi,",
  "tha-ap562:48.2_0": "okkākakulasambhavo;",
  "tha-ap562:48.3_0": "Gotamo nāma gottena,",
  "tha-ap562:48.4_0": "satthā loke bhavissati.",
  "tha-ap562:49.1_0": "Devalokā cavitvāna,",
  "tha-ap562:49.2_0": "sukkamūlena codito;",
  "tha-ap562:49.3_0": "Puññakammena saññutto,",
  "tha-ap562:49.4_0": "brahmabandhu bhavissasi.",
  "tha-ap562:50.1_0": "Asītikoṭiṁ chaḍḍetvā,",
  "tha-ap562:50.2_0": "dāse kammakare bahū;",
  "tha-ap562:50.3_0": "Gotamassa bhagavato,",
  "tha-ap562:50.4_0": "sāsane pabbajissasi.",
  "tha-ap562:51.1_0": "Ārādhayitvā sambuddhaṁ,",
  "tha-ap562:51.2_0": "gotamaṁ sakyapuṅgavaṁ;",
  "tha-ap562:51.3_0": "Upavānoti nāmena,",
  "tha-ap562:51.4_0": "hessasi satthu sāvako’.",
  "tha-ap562:52.1_0": "Satasahasse kataṁ kammaṁ,",
  "tha-ap562:52.2_0": "phalaṁ dassesi me idha;",
  "tha-ap562:52.3_0": "Sumutto saravegova,",
  "tha-ap562:52.4_0": "kilese jhāpayiṁ mama.",
  "tha-ap562:53.1_0": "Cakkavattissa santassa,",
  "tha-ap562:53.2_0": "cātuddīpissarassa me;",
  "tha-ap562:53.3_0": "Tīṇi yojanāni sāmantā,",
  "tha-ap562:53.4_0": "ussīyanti dhajā sadā.",
  "tha-ap562:54.1_0": "Satasahassito kappe,",
  "tha-ap562:54.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap562:54.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap562:54.4_0": "dhajadānassidaṁ phalaṁ.",
  "tha-ap562:55.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap562:55.2_0": "…pe…",
  "tha-ap562:55.3_0": "viharāmi anāsavo.",
  "tha-ap562:56.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap562:56.2_0": "…pe…",
  "tha-ap562:56.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap562:57.1_0": "Paṭisambhidā catasso,",
  "tha-ap562:57.2_0": "…pe…",
  "tha-ap562:57.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap562:58.1_0": "Itthaṁ sudaṁ āyasmā upavānatthero imā gāthāyo abhāsitthāti.",
  "tha-ap562:59.1_0": "Upavānattherassāpadānaṁ dasamaṁ."
}