{
  "ja337:0.1_0": "Jātaka",
  "ja337:0.2_0": "Catukkanipāta",
  "ja337:0.3_0": "Kokilavagga",
  "ja337:0.4_0": "7. Pīṭhajātaka",
  "ja337:1.1_0": "“Na te pīṭhamadāyimhā,",
  "ja337:1.2_0": "na pānaṁ napi bhojanaṁ;",
  "ja337:1.3_0": "Brahmacāri khamassu me,",
  "ja337:1.4_0": "etaṁ passāmi accayaṁ”.",
  "ja337:2.1_0": "“Nevābhisajjāmi na cāpi kuppe,",
  "ja337:2.2_0": "Na cāpi me appiyamāsi kiñci;",
  "ja337:2.3_0": "Athopi me āsi manovitakko,",
  "ja337:2.4_0": "Etādiso nūna kulassa dhammo”.",
  "ja337:3.1_0": "“Esasmākaṁ kule dhammo,",
  "ja337:3.2_0": "pitupitāmaho sadā;",
  "ja337:3.3_0": "Āsanaṁ udakaṁ pajjaṁ,",
  "ja337:3.4_0": "sabbetaṁ nipadāmase.",
  "ja337:4.1_0": "Esasmākaṁ kule dhammo,",
  "ja337:4.2_0": "pitupitāmaho sadā;",
  "ja337:4.3_0": "Sakkaccaṁ upatiṭṭhāma,",
  "ja337:4.4_0": "uttamaṁ viya ñātakan”ti.",
  "ja337:5.1_0": "Pīṭhajātakaṁ sattamaṁ."
}