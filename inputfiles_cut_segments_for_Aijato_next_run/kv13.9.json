{
  "kv13.9:0.1_0": "Kathāvatthu",
  "kv13.9:0.2_0": "Mahāpaṇṇāsaka",
  "kv13.9:0.3_0": "Terasamavagga",
  "kv13.9:0.4_0": "Dhammataṇhāabyākatātikathā",
  "kv13.9:1.1_0": "Dhammataṇhā abyākatāti?",
  "kv13.9:1.2_0": "Āmantā.",
  "kv13.9:1.3_0": "Vipākābyākatā kiriyābyākatā rūpaṁ nibbānaṁ cakkhāyatanaṁ …pe…",
  "kv13.9:1.4_0": "phoṭṭhabbāyatananti?",
  "kv13.9:1.5_0": "Na hevaṁ vattabbe …pe….",
  "kv13.9:2.1_0": "Dhammataṇhā abyākatāti?",
  "kv13.9:2.2_0": "Āmantā.",
  "kv13.9:2.3_0": "Rūpataṇhā abyākatāti?",
  "kv13.9:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv13.9:2.5_0": "dhammataṇhā abyākatāti?",
  "kv13.9:2.6_0": "Āmantā.",
  "kv13.9:2.7_0": "Saddataṇhā …pe…",
  "kv13.9:2.8_0": "gandhataṇhā …pe…",
  "kv13.9:2.9_0": "rasataṇhā …pe…",
  "kv13.9:2.10_0": "phoṭṭhabbataṇhā abyākatāti?",
  "kv13.9:2.11_0": "Na hevaṁ vattabbe …pe….",
  "kv13.9:3.1_0": "Rūpataṇhā akusalāti?",
  "kv13.9:3.2_0": "Āmantā.",
  "kv13.9:3.3_0": "Dhammataṇhā akusalāti?",
  "kv13.9:3.4_0": "Na hevaṁ vattabbe …pe…",
  "kv13.9:3.5_0": "saddataṇhā …pe…",
  "kv13.9:3.6_0": "phoṭṭhabbataṇhā akusalāti?",
  "kv13.9:3.7_0": "Āmantā.",
  "kv13.9:3.8_0": "Dhammataṇhā akusalāti?",
  "kv13.9:3.9_0": "Na hevaṁ vattabbe …pe….",
  "kv13.9:4.1_0": "Dhammataṇhā abyākatāti?",
  "kv13.9:4.2_0": "Āmantā.",
  "kv13.9:4.3_0": "Nanu taṇhā akusalā vuttā bhagavatāti?",
  "kv13.9:4.4_0": "Āmantā.",
  "kv13.9:4.5_0": "Hañci taṇhā akusalā vuttā bhagavatā, no ca vata re vattabbe—",
  "kv13.9:4.6_0": "“dhammataṇhā abyākatā”ti.",
  "kv13.9:5.1_0": "Dhammataṇhā abyākatāti?",
  "kv13.9:5.2_0": "Āmantā.",
  "kv13.9:5.3_0": "Nanu lobho akusalo vutto bhagavatā, dhammataṇhā lobhoti?",
  "kv13.9:5.4_0": "Āmantā.",
  "kv13.9:5.5_0": "Hañci lobho akusalo vutto bhagavatā, dhammataṇhā lobho, no ca vata re vattabbe—",
  "kv13.9:5.6_0": "“dhammataṇhā abyākatā”ti.",
  "kv13.9:6.1_0": "Dhammataṇhā lobho abyākatoti?",
  "kv13.9:6.2_0": "Āmantā.",
  "kv13.9:6.3_0": "Rūpataṇhā lobho abyākatoti?",
  "kv13.9:6.4_0": "Na hevaṁ vattabbe …pe…",
  "kv13.9:6.5_0": "dhammataṇhā lobho abyākatoti?",
  "kv13.9:6.6_0": "Āmantā.",
  "kv13.9:6.7_0": "Saddataṇhā …pe…",
  "kv13.9:6.8_0": "phoṭṭhabbataṇhā lobho abyākatoti?",
  "kv13.9:6.9_0": "Na hevaṁ vattabbe …pe….",
  "kv13.9:7.1_0": "Rūpataṇhā lobho akusaloti?",
  "kv13.9:7.2_0": "Āmantā.",
  "kv13.9:7.3_0": "Dhammataṇhā lobho akusaloti?",
  "kv13.9:7.4_0": "Na hevaṁ vattabbe …pe…",
  "kv13.9:7.5_0": "saddataṇhā …pe…",
  "kv13.9:7.6_0": "phoṭṭhabbataṇhā lobho akusaloti?",
  "kv13.9:7.7_0": "Āmantā.",
  "kv13.9:7.8_0": "Dhammataṇhā lobho akusaloti?",
  "kv13.9:7.9_0": "Na hevaṁ vattabbe …pe….",
  "kv13.9:8.1_0": "Dhammataṇhā abyākatāti?",
  "kv13.9:8.2_0": "Āmantā.",
  "kv13.9:8.3_0": "Nanu vuttaṁ bhagavatā—",
  "kv13.9:8.4_0": "“yāyaṁ taṇhā ponobbhavikā nandīrāgasahagatā tatratatrābhinandinī, seyyathidaṁ—",
  "kv13.9:8.5_0": "kāmataṇhā, bhavataṇhā, vibhavataṇhā”ti.",
  "kv13.9:8.6_0": "Attheva suttantoti?",
  "kv13.9:8.7_0": "Āmantā.",
  "kv13.9:8.8_0": "Tena hi na vattabbaṁ—",
  "kv13.9:8.9_0": "“dhammataṇhā abyākatā”ti.",
  "kv13.9:9.1_0": "Na vattabbaṁ—",
  "kv13.9:9.2_0": "“dhammataṇhā abyākatā”ti?",
  "kv13.9:9.3_0": "Āmantā.",
  "kv13.9:9.4_0": "Nanu sā dhammataṇhāti?",
  "kv13.9:9.5_0": "Āmantā.",
  "kv13.9:9.6_0": "Hañci sā dhammataṇhā, tena vata re vattabbe—",
  "kv13.9:9.7_0": "“dhammataṇhā abyākatā”ti.",
  "kv13.9:9.8_0": "Dhammataṇhā abyākatātikathā niṭṭhitā."
}