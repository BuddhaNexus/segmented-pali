{
  "thag1.62:0.1_0": "Theragāthā",
  "thag1.62:0.2_0": "Ekakanipāta",
  "thag1.62:0.3_0": "Sattamavagga",
  "thag1.62:0.4_0": "1.62. Vajjiputtattheragāthā",
  "thag1.62:1.1_0": "“Ekakā mayaṁ araññe viharāma,",
  "thag1.62:1.2_0": "Apaviddhaṁva vanasmiṁ dārukaṁ;",
  "thag1.62:1.3_0": "Tassa me bahukā pihayanti,",
  "thag1.62:1.4_0": "Nerayikā viya saggagāminan”ti.",
  "thag1.62:1.5_0": "… Vajjiputto thero …."
}