{
  "tha-ap103:0.1_0": "Therāpadāna",
  "tha-ap103:0.2_0": "Bhikkhadāyivagga",
  "tha-ap103:0.3_0": "1 Bhikkhadāyakattheraapadāna",
  "tha-ap103:1.1_0": "“Suvaṇṇavaṇṇaṁ sambuddhaṁ,",
  "tha-ap103:1.2_0": "Āhutīnaṁ paṭiggahaṁ;",
  "tha-ap103:1.3_0": "Pavarā abhinikkhantaṁ,",
  "tha-ap103:1.4_0": "Vanā nibbanamāgataṁ.",
  "tha-ap103:2.1_0": "Kaṭacchubhikkhaṁ pādāsiṁ,",
  "tha-ap103:2.2_0": "siddhatthassa mahesino;",
  "tha-ap103:2.3_0": "Paññāya upasantassa,",
  "tha-ap103:2.4_0": "mahāvīrassa tādino.",
  "tha-ap103:3.1_0": "Padenānupadāyantaṁ,",
  "tha-ap103:3.2_0": "nibbāpente mahājanaṁ;",
  "tha-ap103:3.3_0": "Uḷārā vitti me jātā,",
  "tha-ap103:3.4_0": "buddhe ādiccabandhune.",
  "tha-ap103:4.1_0": "Catunnavutito kappe,",
  "tha-ap103:4.2_0": "yaṁ dānamadadiṁ tadā;",
  "tha-ap103:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap103:4.4_0": "bhikkhādānassidaṁ phalaṁ.",
  "tha-ap103:5.1_0": "Sattāsītimhito kappe,",
  "tha-ap103:5.2_0": "mahāreṇu sanāmakā;",
  "tha-ap103:5.3_0": "Sattaratanasampannā,",
  "tha-ap103:5.4_0": "sattete cakkavattino.",
  "tha-ap103:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap103:6.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap103:6.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap103:6.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap103:7.1_0": "Itthaṁ sudaṁ āyasmā bhikkhadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap103:8.1_0": "Bhikkhadāyakattherassāpadānaṁ paṭhamaṁ."
}