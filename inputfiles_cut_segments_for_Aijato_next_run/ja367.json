{
  "ja367:0.1_0": "Jātaka",
  "ja367:0.2_0": "Pañcakanipāta",
  "ja367:0.3_0": "Vaṇṇārohavagga",
  "ja367:0.4_0": "7. Sāḷiyajātaka",
  "ja367:1.1_0": "“Yvāyaṁ sāḷiyachāpoti,",
  "ja367:1.2_0": "kaṇhasappaṁ agāhayi;",
  "ja367:1.3_0": "Tena sappenayaṁ daṭṭho,",
  "ja367:1.4_0": "hato pāpānusāsako.",
  "ja367:2.1_0": "Ahantāra mahantāraṁ,",
  "ja367:2.2_0": "yo naro hantumicchati;",
  "ja367:2.3_0": "Evaṁ so nihato seti,",
  "ja367:2.4_0": "yathāyaṁ puriso hato.",
  "ja367:3.1_0": "Ahantāra maghātentaṁ,",
  "ja367:3.2_0": "yo naro hantumicchati;",
  "ja367:3.3_0": "Evaṁ so nihato seti,",
  "ja367:3.4_0": "yathāyaṁ puriso hato.",
  "ja367:4.1_0": "Yathā paṁsumuṭṭhiṁ puriso,",
  "ja367:4.2_0": "paṭivātaṁ paṭikkhipe;",
  "ja367:4.3_0": "Tameva so rajo hanti,",
  "ja367:4.4_0": "tathāyaṁ puriso hato.",
  "ja367:5.1_0": "Yo appaduṭṭhassa narassa dussati,",
  "ja367:5.2_0": "Suddhassa posassa anaṅgaṇassa;",
  "ja367:5.3_0": "Tameva bālaṁ pacceti pāpaṁ,",
  "ja367:5.4_0": "Sukhumo rajo paṭivātaṁva khitto”ti.",
  "ja367:6.1_0": "Sāḷiyajātakaṁ sattamaṁ."
}