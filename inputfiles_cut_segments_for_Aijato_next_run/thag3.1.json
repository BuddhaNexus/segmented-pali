{
  "thag3.1:0.1_0": "Theragāthā",
  "thag3.1:0.2_0": "Tikanipāta",
  "thag3.1:0.3_0": "Paṭhamavagga",
  "thag3.1:0.4_0": "3.1 Aṅgaṇikabhāradvājattheragāthā",
  "thag3.1:1.1_0": "“Ayoni suddhimanvesaṁ,",
  "thag3.1:1.2_0": "aggiṁ paricariṁ vane;",
  "thag3.1:1.3_0": "Suddhimaggaṁ ajānanto,",
  "thag3.1:1.4_0": "akāsiṁ amaraṁ tapaṁ.",
  "thag3.1:2.1_0": "Taṁ sukhena sukhaṁ laddhaṁ,",
  "thag3.1:2.2_0": "passa dhammasudhammataṁ;",
  "thag3.1:2.3_0": "Tisso vijjā anuppattā,",
  "thag3.1:2.4_0": "kataṁ buddhassa sāsanaṁ.",
  "thag3.1:3.1_0": "Brahmabandhu pure āsiṁ,",
  "thag3.1:3.2_0": "idāni khomhi brāhmaṇo;",
  "thag3.1:3.3_0": "Tevijjo nhātako camhi,",
  "thag3.1:3.4_0": "sottiyo camhi vedagū”ti.",
  "thag3.1:3.5_0": "… Aṅgaṇikabhāradvājo thero …."
}