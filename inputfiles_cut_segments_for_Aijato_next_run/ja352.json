{
  "ja352:0.1_0": "Jātaka",
  "ja352:0.2_0": "Pañcakanipāta",
  "ja352:0.3_0": "Maṇikuṇḍalavagga",
  "ja352:0.4_0": "2. Sujātajātaka",
  "ja352:1.1_0": "“Kiṁ nu santaramānova,",
  "ja352:1.2_0": "lāyitvā haritaṁ tiṇaṁ;",
  "ja352:1.3_0": "Khāda khādāti lapasi,",
  "ja352:1.4_0": "gatasattaṁ jaraggavaṁ.",
  "ja352:2.1_0": "Na hi annena pānena,",
  "ja352:2.2_0": "mato goṇo samuṭṭhahe;",
  "ja352:2.3_0": "Tvañca tucchaṁ vilapasi,",
  "ja352:2.4_0": "yathā taṁ dummatī tathā”.",
  "ja352:3.1_0": "“Tatheva tiṭṭhati sīsaṁ,",
  "ja352:3.2_0": "hatthapādā ca vāladhi;",
  "ja352:3.3_0": "Sotā tatheva tiṭṭhanti,",
  "ja352:3.4_0": "maññe goṇo samuṭṭhahe.",
  "ja352:4.1_0": "Nevayyakassa sīsañca,",
  "ja352:4.2_0": "hatthapādā ca dissare;",
  "ja352:4.3_0": "Rudaṁ mattikathūpasmiṁ,",
  "ja352:4.4_0": "nanu tvaññeva dummati”.",
  "ja352:5.1_0": "“Ādittaṁ vata maṁ santaṁ,",
  "ja352:5.2_0": "ghatasittaṁva pāvakaṁ;",
  "ja352:5.3_0": "Vārinā viya osiñcaṁ,",
  "ja352:5.4_0": "sabbaṁ nibbāpaye daraṁ.",
  "ja352:6.1_0": "Abbahī vata me sallaṁ,",
  "ja352:6.2_0": "yamāsi hadayassitaṁ;",
  "ja352:6.3_0": "Yo me sokaparetassa,",
  "ja352:6.4_0": "pitusokaṁ apānudi.",
  "ja352:7.1_0": "Sohaṁ abbūḷhasallosmi,",
  "ja352:7.2_0": "vītasoko anāvilo;",
  "ja352:7.3_0": "Na socāmi na rodāmi,",
  "ja352:7.4_0": "tava sutvāna māṇava.",
  "ja352:8.1_0": "Evaṁ karonti sappaññā,",
  "ja352:8.2_0": "ye honti anukampakā;",
  "ja352:8.3_0": "Vinivattenti sokamhā,",
  "ja352:8.4_0": "sujāto pitaraṁ yathā”ti.",
  "ja352:9.1_0": "Sujātajātakaṁ dutiyaṁ."
}