{
  "ja142:0.1_0": "Jātaka",
  "ja142:0.2_0": "Ekakanipāta",
  "ja142:0.3_0": "Kakaṇṭakavagga",
  "ja142:0.4_0": "2. Siṅgālajātaka",
  "ja142:1.1_0": "“Etañhi te durājānaṁ,",
  "ja142:1.2_0": "yaṁ sesi matasāyikaṁ;",
  "ja142:1.3_0": "Yassa te kaḍḍhamānassa,",
  "ja142:1.4_0": "hatthā daṇḍo na muccatī”ti.",
  "ja142:2.1_0": "Siṅgālajātakaṁ dutiyaṁ."
}