{
  "tha-ap432:0.1_0": "Therāpadāna",
  "tha-ap432:0.2_0": "Sakiṁsammajjakavagga",
  "tha-ap432:0.3_0": "10 Soṇṇakontarikattheraapadāna",
  "tha-ap432:1.1_0": "“Manobhāvaniyaṁ buddhaṁ,",
  "tha-ap432:1.2_0": "attadantaṁ samāhitaṁ;",
  "tha-ap432:1.3_0": "Iriyamānaṁ brahmapathe,",
  "tha-ap432:1.4_0": "cittavūpasame rataṁ.",
  "tha-ap432:2.1_0": "Nittiṇṇaoghaṁ sambuddhaṁ,",
  "tha-ap432:2.2_0": "jhāyiṁ jhānarataṁ muniṁ;",
  "tha-ap432:2.3_0": "Upatitthaṁ samāpannaṁ,",
  "tha-ap432:2.4_0": "indivaradalappabhaṁ.",
  "tha-ap432:3.1_0": "Alābunodakaṁ gayha,",
  "tha-ap432:3.2_0": "buddhaseṭṭhaṁ upāgamiṁ;",
  "tha-ap432:3.3_0": "Buddhassa pāde dhovitvā,",
  "tha-ap432:3.4_0": "alābukamadāsahaṁ.",
  "tha-ap432:4.1_0": "Āṇāpesi ca sambuddho,",
  "tha-ap432:4.2_0": "padumuttaranāmako;",
  "tha-ap432:4.3_0": "‘Iminā dakamāhatvā,",
  "tha-ap432:4.4_0": "pādamūle ṭhapehi me’.",
  "tha-ap432:5.1_0": "Sādhūtihaṁ paṭissutvā,",
  "tha-ap432:5.2_0": "satthugāravatāya ca;",
  "tha-ap432:5.3_0": "Dakaṁ alābunāhatvā,",
  "tha-ap432:5.4_0": "buddhaseṭṭhaṁ upāgamiṁ.",
  "tha-ap432:6.1_0": "Anumodi mahāvīro,",
  "tha-ap432:6.2_0": "cittaṁ nibbāpayaṁ mama;",
  "tha-ap432:6.3_0": "‘Iminālābudānena,",
  "tha-ap432:6.4_0": "saṅkappo te samijjhatu’.",
  "tha-ap432:7.1_0": "Pannarasesu kappesu,",
  "tha-ap432:7.2_0": "devaloke ramiṁ ahaṁ;",
  "tha-ap432:7.3_0": "Tiṁsatikkhattuṁ rājā ca,",
  "tha-ap432:7.4_0": "cakkavattī ahosahaṁ.",
  "tha-ap432:8.1_0": "Divā vā yadi vā rattiṁ,",
  "tha-ap432:8.2_0": "caṅkamantassa tiṭṭhato;",
  "tha-ap432:8.3_0": "Sovaṇṇaṁ kontaraṁ gayha,",
  "tha-ap432:8.4_0": "tiṭṭhate purato mama.",
  "tha-ap432:9.1_0": "Buddhassa datvānalābuṁ,",
  "tha-ap432:9.2_0": "labhāmi soṇṇakontaraṁ;",
  "tha-ap432:9.3_0": "Appakampi kataṁ kāraṁ,",
  "tha-ap432:9.4_0": "vipulaṁ hoti tādisu.",
  "tha-ap432:10.1_0": "Satasahassito kappe,",
  "tha-ap432:10.2_0": "yaṁlābumadadiṁ tadā;",
  "tha-ap432:10.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap432:10.4_0": "alābussa idaṁ phalaṁ.",
  "tha-ap432:11.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap432:11.2_0": "…pe…",
  "tha-ap432:11.3_0": "viharāmi anāsavo.",
  "tha-ap432:12.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap432:12.2_0": "…pe…",
  "tha-ap432:12.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap432:13.1_0": "Paṭisambhidā catasso,",
  "tha-ap432:13.2_0": "…pe…",
  "tha-ap432:13.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap432:14.1_0": "Itthaṁ sudaṁ āyasmā soṇṇakontariko thero imā gāthāyo abhāsitthāti.",
  "tha-ap432:15.1_0": "Soṇṇakontarikattherassāpadānaṁ dasamaṁ.",
  "tha-ap432:16.1_0": "Sakiṁsammajjakavaggo tecattālīsamo.",
  "tha-ap432:17.1_0": "Tassuddānaṁ",
  "tha-ap432:18.1_0": "Sakiṁsammajjako thero,",
  "tha-ap432:18.2_0": "ekadussī ekāsanī;",
  "tha-ap432:18.3_0": "Kadambakoraṇḍakado,",
  "tha-ap432:18.4_0": "ghatassavanikopi ca.",
  "tha-ap432:19.1_0": "Sucintiko kiṅkaṇiko,",
  "tha-ap432:19.2_0": "soṇṇakontarikopi ca;",
  "tha-ap432:19.3_0": "Ekagāthāsatañcettha,",
  "tha-ap432:19.4_0": "ekasattatimeva ca."
}