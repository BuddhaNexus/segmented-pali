{
  "thag2.44:0.1_0": "Theragāthā",
  "thag2.44:0.2_0": "Dukanipāta",
  "thag2.44:0.3_0": "Pañcamavagga",
  "thag2.44:0.4_0": "2.44. Mogharājattheragāthā",
  "thag2.44:1.1_0": "“Chavipāpaka cittabhaddaka,",
  "thag2.44:1.2_0": "Mogharāja satataṁ samāhito;",
  "thag2.44:1.3_0": "Hemantikasītakālarattiyo,",
  "thag2.44:1.4_0": "Bhikkhu tvaṁsi kathaṁ karissasi”.",
  "thag2.44:2.1_0": "“Sampannasassā magadhā,",
  "thag2.44:2.2_0": "kevalā iti me sutaṁ;",
  "thag2.44:2.3_0": "Palālacchannako seyyaṁ,",
  "thag2.44:2.4_0": "yathaññe sukhajīvino”ti.",
  "thag2.44:2.5_0": "… Mogharājā thero …."
}