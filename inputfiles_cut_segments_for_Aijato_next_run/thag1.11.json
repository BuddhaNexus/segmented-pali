{
  "thag1.11:0.1_0": "Theragāthā",
  "thag1.11:0.2_0": "Ekakanipāta",
  "thag1.11:0.3_0": "Dutiyavagga",
  "thag1.11:0.4_0": "1.11. Cūḷavacchattheragāthā",
  "thag1.11:1.1_0": "“Pāmojjabahulo bhikkhu,",
  "thag1.11:1.2_0": "dhamme buddhappavedite;",
  "thag1.11:1.3_0": "Adhigacche padaṁ santaṁ,",
  "thag1.11:1.4_0": "saṅkhārūpasamaṁ sukhan”ti.",
  "thag1.11:1.5_0": "… Cūḷavaccho thero …."
}