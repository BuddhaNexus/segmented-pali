{
  "ja221:0.1_0": "Jātaka",
  "ja221:0.2_0": "Dukanipāta",
  "ja221:0.3_0": "Kāsāvavagga",
  "ja221:0.4_0": "1. Kāsāvajātaka",
  "ja221:1.1_0": "“Anikkasāvo kāsāvaṁ,",
  "ja221:1.2_0": "yo vatthaṁ paridahissati;",
  "ja221:1.3_0": "Apeto damasaccena,",
  "ja221:1.4_0": "na so kāsāvamarahati.",
  "ja221:2.1_0": "Yo ca vantakasāvassa,",
  "ja221:2.2_0": "Sīlesu susamāhito;",
  "ja221:2.3_0": "Upeto damasaccena,",
  "ja221:2.4_0": "Sa ve kāsāvamarahatī”ti.",
  "ja221:3.1_0": "Kāsāvajātakaṁ paṭhamaṁ."
}