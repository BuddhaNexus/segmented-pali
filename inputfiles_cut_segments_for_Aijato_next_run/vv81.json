{
  "vv81:0.1_0": "Vimānavatthu",
  "vv81:0.2_0": "Purisavimāna",
  "vv81:0.3_0": "Sunikkhittavagga",
  "vv81:0.4_0": "7. Kaṇḍakavimānavatthu",
  "vv81:1.1_0": "“Puṇṇamāse yathā cando,",
  "vv81:1.2_0": "nakkhattaparivārito;",
  "vv81:1.3_0": "Samantā anupariyāti,",
  "vv81:1.4_0": "tārakādhipatī sasī.",
  "vv81:2.1_0": "Tathūpamaṁ idaṁ byamhaṁ,",
  "vv81:2.2_0": "dibbaṁ devapuramhi ca;",
  "vv81:2.3_0": "Atirocati vaṇṇena,",
  "vv81:2.4_0": "udayantova raṁsimā.",
  "vv81:3.1_0": "Veḷuriyasuvaṇṇassa,",
  "vv81:3.2_0": "phalikā rūpiyassa ca;",
  "vv81:3.3_0": "Masāragallamuttāhi,",
  "vv81:3.4_0": "lohitaṅgamaṇīhi ca.",
  "vv81:4.1_0": "Citrā manoramā bhūmi,",
  "vv81:4.2_0": "veḷūriyassa santhatā;",
  "vv81:4.3_0": "Kūṭāgārā subhā rammā,",
  "vv81:4.4_0": "pāsādo te sumāpito.",
  "vv81:5.1_0": "Rammā ca te pokkharaṇī,",
  "vv81:5.2_0": "puthulomanisevitā;",
  "vv81:5.3_0": "Acchodakā vippasannā,",
  "vv81:5.4_0": "sovaṇṇavālukasanthatā.",
  "vv81:6.1_0": "Nānāpadumasañchannā,",
  "vv81:6.2_0": "puṇḍarīkasamotatā;",
  "vv81:6.3_0": "Surabhiṁ sampavāyanti,",
  "vv81:6.4_0": "manuññā māluteritā.",
  "vv81:7.1_0": "Tassā te ubhato passe,",
  "vv81:7.2_0": "vanagumbā sumāpitā;",
  "vv81:7.3_0": "Upetā puppharukkhehi,",
  "vv81:7.4_0": "phalarukkhehi cūbhayaṁ.",
  "vv81:8.1_0": "Sovaṇṇapāde pallaṅke,",
  "vv81:8.2_0": "muduke gonakatthate;",
  "vv81:8.3_0": "Nisinnaṁ devarājaṁva,",
  "vv81:8.4_0": "upatiṭṭhanti accharā.",
  "vv81:9.1_0": "Sabbābharaṇasañchannā,",
  "vv81:9.2_0": "nānāmālāvibhūsitā;",
  "vv81:9.3_0": "Ramenti taṁ mahiddhikaṁ,",
  "vv81:9.4_0": "vasavattīva modasi.",
  "vv81:10.1_0": "Bherisaṅkhamudiṅgāhi,",
  "vv81:10.2_0": "vīṇāhi paṇavehi ca;",
  "vv81:10.3_0": "Ramasi ratisampanno,",
  "vv81:10.4_0": "naccagīte suvādite.",
  "vv81:11.1_0": "Dibbā te vividhā rūpā,",
  "vv81:11.2_0": "dibbā saddā atho rasā;",
  "vv81:11.3_0": "Gandhā ca te adhippetā,",
  "vv81:11.4_0": "phoṭṭhabbā ca manoramā.",
  "vv81:12.1_0": "Tasmiṁ vimāne pavare,",
  "vv81:12.2_0": "devaputta mahappabho;",
  "vv81:12.3_0": "Atirocasi vaṇṇena,",
  "vv81:12.4_0": "udayantova bhāṇumā.",
  "vv81:13.1_0": "Dānassa te idaṁ phalaṁ,",
  "vv81:13.2_0": "atho sīlassa vā pana;",
  "vv81:13.3_0": "Atho añjalikammassa,",
  "vv81:13.4_0": "taṁ me akkhāhi pucchito”.",
  "vv81:14.1_0": "So devaputto attamano,",
  "vv81:14.2_0": "…pe…",
  "vv81:14.3_0": "yassa kammassidaṁ phalaṁ.",
  "vv81:15.1_0": "“Ahaṁ kapilavatthusmiṁ,",
  "vv81:15.2_0": "sākiyānaṁ puruttame;",
  "vv81:15.3_0": "Suddhodanassa puttassa,",
  "vv81:15.4_0": "kaṇḍako sahajo ahaṁ.",
  "vv81:16.1_0": "Yadā so aḍḍharattāyaṁ,",
  "vv81:16.2_0": "bodhāya mabhinikkhami;",
  "vv81:16.3_0": "So maṁ mudūhi pāṇīhi,",
  "vv81:16.4_0": "jāli tambanakhehi ca.",
  "vv81:17.1_0": "Satthiṁ ākoṭayitvāna,",
  "vv81:17.2_0": "vaha sammāti cabravi;",
  "vv81:17.3_0": "Ahaṁ lokaṁ tārayissaṁ,",
  "vv81:17.4_0": "patto sambodhimuttamaṁ.",
  "vv81:18.1_0": "Taṁ me giraṁ suṇantassa,",
  "vv81:18.2_0": "hāso me vipulo ahu;",
  "vv81:18.3_0": "Udaggacitto sumano,",
  "vv81:18.4_0": "abhisīsiṁ tadā ahaṁ.",
  "vv81:19.1_0": "Abhirūḷhañca maṁ ñatvā,",
  "vv81:19.2_0": "sakyaputtaṁ mahāyasaṁ;",
  "vv81:19.3_0": "Udaggacitto mudito,",
  "vv81:19.4_0": "vahissaṁ purisuttamaṁ.",
  "vv81:20.1_0": "Paresaṁ vijitaṁ gantvā,",
  "vv81:20.2_0": "uggatasmiṁ divākare;",
  "vv81:20.3_0": "Mamaṁ channañca ohāya,",
  "vv81:20.4_0": "anapekkho so apakkami.",
  "vv81:21.1_0": "Tassa tambanakhe pāde,",
  "vv81:21.2_0": "jivhāya parilehisaṁ;",
  "vv81:21.3_0": "Gacchantañca mahāvīraṁ,",
  "vv81:21.4_0": "rudamāno udikkhisaṁ.",
  "vv81:22.1_0": "Adassanenahaṁ tassa,",
  "vv81:22.2_0": "sakyaputtassa sirīmato;",
  "vv81:22.3_0": "Alatthaṁ garukābādhaṁ,",
  "vv81:22.4_0": "khippaṁ me maraṇaṁ ahu.",
  "vv81:23.1_0": "Tasseva ānubhāvena,",
  "vv81:23.2_0": "vimānaṁ āvasāmidaṁ;",
  "vv81:23.3_0": "Sabbakāmaguṇopetaṁ,",
  "vv81:23.4_0": "dibbaṁ devapuramhi ca.",
  "vv81:24.1_0": "Yañca me ahuvā hāso,",
  "vv81:24.2_0": "saddaṁ sutvāna bodhiyā;",
  "vv81:24.3_0": "Teneva kusalamūlena,",
  "vv81:24.4_0": "phusissaṁ āsavakkhayaṁ.",
  "vv81:25.1_0": "Sace hi bhante gaccheyyāsi,",
  "vv81:25.2_0": "satthu buddhassa santike;",
  "vv81:25.3_0": "Mamāpi naṁ vacanena,",
  "vv81:25.4_0": "sirasā vajjāsi vandanaṁ.",
  "vv81:26.1_0": "Ahampi daṭṭhuṁ gacchissaṁ,",
  "vv81:26.2_0": "jinaṁ appaṭipuggalaṁ;",
  "vv81:26.3_0": "Dullabhaṁ dassanaṁ hoti,",
  "vv81:26.4_0": "lokanāthāna tādinan”ti.",
  "vv81:27.1_0": "So kataññū katavedī,",
  "vv81:27.2_0": "satthāraṁ upasaṅkami;",
  "vv81:27.3_0": "Sutvā giraṁ cakkhumato,",
  "vv81:27.4_0": "dhammacakkhuṁ visodhayi.",
  "vv81:28.1_0": "Visodhetvā diṭṭhigataṁ,",
  "vv81:28.2_0": "vicikicchaṁ vatāni ca;",
  "vv81:28.3_0": "Vanditvā satthuno pāde,",
  "vv81:28.4_0": "tatthevantaradhāyathāti.",
  "vv81:29.1_0": "Kaṇḍakavimānaṁ sattamaṁ."
}