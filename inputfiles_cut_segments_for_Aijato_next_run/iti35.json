{
  "iti35:0.1_0": "Itivuttaka 35",
  "iti35:0.2_0": "Dukanipāta",
  "iti35:0.3_0": "Paṭhamavagga",
  "iti35:0.4_0": "Paṭhamajananakuhanasutta",
  "iti35:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti35:2.1_0": "“Nayidaṁ, bhikkhave, brahmacariyaṁ vussati janakuhanatthaṁ, na janalapanatthaṁ, na lābhasakkārasilokānisaṁsatthaṁ, na ‘iti maṁ jano jānātū’ti.",
  "iti35:2.2_0": "Atha kho idaṁ, bhikkhave, brahmacariyaṁ vussati saṁvaratthañceva pahānatthañcā”ti.",
  "iti35:2.3_0": "Etamatthaṁ bhagavā avoca.",
  "iti35:2.4_0": "Tatthetaṁ iti vuccati:",
  "iti35:3.1_0": "“Saṁvaratthaṁ pahānatthaṁ,",
  "iti35:3.2_0": "brahmacariyaṁ anītihaṁ;",
  "iti35:3.3_0": "Adesayi so bhagavā,",
  "iti35:3.4_0": "nibbānogadhagāminaṁ.",
  "iti35:4.1_0": "Esa maggo mahattehi,",
  "iti35:4.2_0": "anuyāto mahesibhi;",
  "iti35:4.3_0": "Ye ye taṁ paṭipajjanti,",
  "iti35:4.4_0": "yathā buddhena desitaṁ;",
  "iti35:4.5_0": "Dukkhassantaṁ karissanti,",
  "iti35:4.6_0": "satthusāsanakārino”ti.",
  "iti35:5.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti35:5.2_0": "Aṭṭhamaṁ."
}