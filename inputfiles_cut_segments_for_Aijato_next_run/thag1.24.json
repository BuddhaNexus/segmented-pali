{
  "thag1.24:0.1_0": "Theragāthā",
  "thag1.24:0.2_0": "Ekakanipāta",
  "thag1.24:0.3_0": "Tatiyavagga",
  "thag1.24:0.4_0": "1.24. Sugandhattheragāthā",
  "thag1.24:1.1_0": "“Anuvassiko pabbajito,",
  "thag1.24:1.2_0": "passa dhammasudhammataṁ;",
  "thag1.24:1.3_0": "Tisso vijjā anuppattā,",
  "thag1.24:1.4_0": "kataṁ buddhassa sāsanan”ti.",
  "thag1.24:1.5_0": "… Sugandho thero …."
}