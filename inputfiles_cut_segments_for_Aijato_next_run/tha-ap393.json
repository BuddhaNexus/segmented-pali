{
  "tha-ap393:0.1_0": "Therāpadāna",
  "tha-ap393:0.2_0": "Pilindavacchavagga",
  "tha-ap393:0.3_0": "1. Pilindavacchattheraapadāna",
  "tha-ap393:1.1_0": "“Nagare haṁsavatiyā,",
  "tha-ap393:1.2_0": "āsiṁ dovāriko ahaṁ;",
  "tha-ap393:1.3_0": "Akkhobhaṁ amitaṁ bhogaṁ,",
  "tha-ap393:1.4_0": "ghare sannicitaṁ mama.",
  "tha-ap393:2.1_0": "Rahogato nisīditvā,",
  "tha-ap393:2.2_0": "pahaṁsitvāna mānasaṁ;",
  "tha-ap393:2.3_0": "Nisajja pāsādavare,",
  "tha-ap393:2.4_0": "evaṁ cintesahaṁ tadā.",
  "tha-ap393:3.0_0": "1.1. Cintanākāra",
  "tha-ap393:3.1_0": "‘Bahū medhigatā bhogā,",
  "tha-ap393:3.2_0": "phītaṁ antepuraṁ mama;",
  "tha-ap393:3.3_0": "Rājāpi sannimantesi,",
  "tha-ap393:3.4_0": "ānando pathavissaro.",
  "tha-ap393:4.1_0": "Ayañca buddho uppanno,",
  "tha-ap393:4.2_0": "adhiccuppattiko muni;",
  "tha-ap393:4.3_0": "Saṁvijjanti ca me bhogā,",
  "tha-ap393:4.4_0": "dānaṁ dassāmi satthuno.",
  "tha-ap393:5.1_0": "Padumena rājaputtena,",
  "tha-ap393:5.2_0": "dinnaṁ dānavaraṁ jine;",
  "tha-ap393:5.3_0": "Hatthināge ca pallaṅke,",
  "tha-ap393:5.4_0": "apassenañcanappakaṁ.",
  "tha-ap393:6.1_0": "Ahampi dānaṁ dassāmi,",
  "tha-ap393:6.2_0": "saṅghe gaṇavaruttame;",
  "tha-ap393:6.3_0": "Adinnapubbamaññesaṁ,",
  "tha-ap393:6.4_0": "bhavissaṁ ādikammiko.",
  "tha-ap393:7.1_0": "Cintetvāhaṁ bahuvidhaṁ,",
  "tha-ap393:7.2_0": "yāge yassa sukhaṁphalaṁ;",
  "tha-ap393:7.3_0": "Parikkhāradānamaddakkhiṁ,",
  "tha-ap393:7.4_0": "mama saṅkappapūraṇaṁ.",
  "tha-ap393:8.1_0": "Parikkhārāni dassāmi,",
  "tha-ap393:8.2_0": "saṅghe gaṇavaruttame;",
  "tha-ap393:8.3_0": "Adinnapubbamaññesaṁ,",
  "tha-ap393:8.4_0": "bhavissaṁ ādikammiko’.",
  "tha-ap393:9.0_0": "1.2. Dānavatthusampādana",
  "tha-ap393:9.1_0": "Naḷakāre upāgamma,",
  "tha-ap393:9.2_0": "chattaṁ kāresi tāvade;",
  "tha-ap393:9.3_0": "Chattasatasahassāni,",
  "tha-ap393:9.4_0": "ekato sannipātayiṁ.",
  "tha-ap393:10.1_0": "Dussasatasahassāni,",
  "tha-ap393:10.2_0": "ekato sannipātayiṁ;",
  "tha-ap393:10.3_0": "Pattasatasahassāni,",
  "tha-ap393:10.4_0": "ekato sannipātayiṁ.",
  "tha-ap393:11.1_0": "Vāsiyo satthake cāpi,",
  "tha-ap393:11.2_0": "sūciyo nakhachedane;",
  "tha-ap393:11.3_0": "Heṭṭhāchatte ṭhapāpesiṁ,",
  "tha-ap393:11.4_0": "kāretvā tadanucchave.",
  "tha-ap393:12.1_0": "Vidhūpane tālavaṇṭe,",
  "tha-ap393:12.2_0": "morahatthe ca cāmare;",
  "tha-ap393:12.3_0": "Parissāvane teladhāre,",
  "tha-ap393:12.4_0": "kārayiṁ tadanucchave.",
  "tha-ap393:13.1_0": "Sūcighare aṁsabaddhe,",
  "tha-ap393:13.2_0": "athopi kāyabandhane;",
  "tha-ap393:13.3_0": "Ādhārake ca sukate,",
  "tha-ap393:13.4_0": "kārayiṁ tadanucchave.",
  "tha-ap393:14.1_0": "Paribhogabhājane ca,",
  "tha-ap393:14.2_0": "athopi lohathālake;",
  "tha-ap393:14.3_0": "Bhesajje pūrayitvāna,",
  "tha-ap393:14.4_0": "heṭṭhāchatte ṭhapesahaṁ.",
  "tha-ap393:15.1_0": "Vacaṁ usīraṁ laṭṭhimadhuṁ,",
  "tha-ap393:15.2_0": "pipphalī maricāni ca;",
  "tha-ap393:15.3_0": "Harītakiṁ siṅgīveraṁ,",
  "tha-ap393:15.4_0": "sabbaṁ pūresi bhājane.",
  "tha-ap393:16.1_0": "Upāhanā pādukāyo,",
  "tha-ap393:16.2_0": "atho udakapuñchane;",
  "tha-ap393:16.3_0": "Kattaradaṇḍe sukate,",
  "tha-ap393:16.4_0": "kārayiṁ tadanucchave.",
  "tha-ap393:17.1_0": "Osadhañjananāḷī ca,",
  "tha-ap393:17.2_0": "salākā dhammakuttarā;",
  "tha-ap393:17.3_0": "Kuñcikā pañcavaṇṇehi,",
  "tha-ap393:17.4_0": "sibbite kuñcikāghare.",
  "tha-ap393:18.1_0": "Āyoge dhūmanette ca,",
  "tha-ap393:18.2_0": "athopi dīpadhārake;",
  "tha-ap393:18.3_0": "Tumbake ca karaṇḍe ca,",
  "tha-ap393:18.4_0": "kārayiṁ tadanucchave.",
  "tha-ap393:19.1_0": "Saṇḍāse pipphale ceva,",
  "tha-ap393:19.2_0": "athopi malahārake;",
  "tha-ap393:19.3_0": "Bhesajjathavike ceva,",
  "tha-ap393:19.4_0": "kārayiṁ tadanucchave.",
  "tha-ap393:20.1_0": "Āsandiyo pīṭhake ca,",
  "tha-ap393:20.2_0": "pallaṅke caturomaye;",
  "tha-ap393:20.3_0": "Tadanucchave kārayitvā,",
  "tha-ap393:20.4_0": "heṭṭhāchatte ṭhapesahaṁ.",
  "tha-ap393:21.1_0": "Uṇṇābhisī tūlabhisī,",
  "tha-ap393:21.2_0": "athopi pīṭhikābhisī;",
  "tha-ap393:21.3_0": "Bimbohane ca sukate,",
  "tha-ap393:21.4_0": "kārayiṁ tadanucchave.",
  "tha-ap393:22.1_0": "Kuruvinde madhusitthe,",
  "tha-ap393:22.2_0": "telaṁ hatthappatāpakaṁ;",
  "tha-ap393:22.3_0": "Sipāṭiphalake sucī,",
  "tha-ap393:22.4_0": "mañcaṁ attharaṇena ca.",
  "tha-ap393:23.1_0": "Senāsane pādapuñche,",
  "tha-ap393:23.2_0": "sayanāsanadaṇḍake;",
  "tha-ap393:23.3_0": "Dantapoṇe ca āṭalī,",
  "tha-ap393:23.4_0": "sīsālepanagandhake.",
  "tha-ap393:24.1_0": "Araṇī phalapīṭhe ca,",
  "tha-ap393:24.2_0": "pattapidhānathālake;",
  "tha-ap393:24.3_0": "Udakassa kaṭacchū ca,",
  "tha-ap393:24.4_0": "cuṇṇakaṁ rajanambaṇaṁ.",
  "tha-ap393:25.1_0": "Sammajjanaṁ udapattaṁ,",
  "tha-ap393:25.2_0": "tathā vassikasāṭikaṁ;",
  "tha-ap393:25.3_0": "Nisīdanaṁ kaṇḍucchādi,",
  "tha-ap393:25.4_0": "atha antaravāsakaṁ.",
  "tha-ap393:26.1_0": "Uttarāsaṅgasaṅghāṭī,",
  "tha-ap393:26.2_0": "natthukaṁ mukhasodhanaṁ;",
  "tha-ap393:26.3_0": "Biḷaṅgaloṇaṁ pahūtañca,",
  "tha-ap393:26.4_0": "madhuñca dadhipānakaṁ.",
  "tha-ap393:27.1_0": "Dhūpaṁ sitthaṁ pilotiñca,",
  "tha-ap393:27.2_0": "mukhapuñchanasuttakaṁ;",
  "tha-ap393:27.3_0": "Dātabbaṁ nāma yaṁ atthi,",
  "tha-ap393:27.4_0": "yañca kappati satthuno.",
  "tha-ap393:28.1_0": "Sabbametaṁ samānetvā,",
  "tha-ap393:28.2_0": "ānandaṁ upasaṅkamiṁ;",
  "tha-ap393:28.3_0": "Upasaṅkamma rājānaṁ,",
  "tha-ap393:28.4_0": "janetāraṁ mahesino;",
  "tha-ap393:28.5_0": "Sirasā abhivādetvā,",
  "tha-ap393:28.6_0": "idaṁ vacanamabraviṁ.",
  "tha-ap393:29.0_0": "1.3. Dānokāsayācanā",
  "tha-ap393:29.1_0": "‘Ekato jātasaṁvaddhā,",
  "tha-ap393:29.2_0": "ubhinnaṁ ekato manaṁ;",
  "tha-ap393:29.3_0": "Sādhāraṇā sukhadukkhe,",
  "tha-ap393:29.4_0": "ubho ca anuvattakā.",
  "tha-ap393:30.1_0": "Atthi cetasikaṁ dukkhaṁ,",
  "tha-ap393:30.2_0": "tavādheyyaṁ arindama;",
  "tha-ap393:30.3_0": "Yadi sakkosi taṁ dukkhaṁ,",
  "tha-ap393:30.4_0": "vinodeyyāsi khattiya’.",
  "tha-ap393:31.1_0": "‘Tava dukkhaṁ mama dukkhaṁ,",
  "tha-ap393:31.2_0": "Ubhinnaṁ ekato mano;",
  "tha-ap393:31.3_0": "Niṭṭhitanti vijānāhi,",
  "tha-ap393:31.4_0": "Mamādheyyaṁ sace tuvaṁ’.",
  "tha-ap393:32.1_0": "‘Jānāhi kho mahārāja,",
  "tha-ap393:32.2_0": "dukkhaṁ me dubbinodayaṁ;",
  "tha-ap393:32.3_0": "Pahu samāno gajjasu,",
  "tha-ap393:32.4_0": "ekaṁ te duccajaṁ varaṁ.",
  "tha-ap393:33.1_0": "Yāvatā vijite atthi,",
  "tha-ap393:33.2_0": "yāvatā mama jīvitaṁ;",
  "tha-ap393:33.3_0": "Etehi yadi te attho,",
  "tha-ap393:33.4_0": "dassāmi avikampito.",
  "tha-ap393:34.1_0": "Gajjitaṁ kho tayā deva,",
  "tha-ap393:34.2_0": "micchā taṁ bahu gajjitaṁ;",
  "tha-ap393:34.3_0": "Jānissāmi tuvaṁ ajja,",
  "tha-ap393:34.4_0": "sabbadhamme patiṭṭhitaṁ’.",
  "tha-ap393:35.1_0": "‘Atibāḷhaṁ nipīḷesi,",
  "tha-ap393:35.2_0": "dadamānassa me sato;",
  "tha-ap393:35.3_0": "Kiṁ te me pīḷitenattho,",
  "tha-ap393:35.4_0": "patthitaṁ te kathehi me’.",
  "tha-ap393:36.1_0": "‘Icchāmahaṁ mahārāja,",
  "tha-ap393:36.2_0": "buddhaseṭṭhaṁ anuttaraṁ;",
  "tha-ap393:36.3_0": "Bhojayissāmi sambuddhaṁ,",
  "tha-ap393:36.4_0": "vajjaṁ me māhu jīvitaṁ’.",
  "tha-ap393:37.1_0": "‘Aññaṁ tehaṁ varaṁ dammi,",
  "tha-ap393:37.2_0": "mā yācittho tathāgataṁ;",
  "tha-ap393:37.3_0": "Adeyyo kassaci buddho,",
  "tha-ap393:37.4_0": "maṇi jotiraso yathā’.",
  "tha-ap393:38.1_0": "‘Nanu te gajjitaṁ deva,",
  "tha-ap393:38.2_0": "yāva jīvitamattano;",
  "tha-ap393:38.3_0": "Jīvitaṁ dadamānena,",
  "tha-ap393:38.4_0": "yuttaṁ dātuṁ tathāgataṁ’.",
  "tha-ap393:39.1_0": "‘Ṭhapanīyo mahāvīro,",
  "tha-ap393:39.2_0": "adeyyo kassaci jino;",
  "tha-ap393:39.3_0": "Na me paṭissuto buddho,",
  "tha-ap393:39.4_0": "varassu amitaṁ dhanaṁ’.",
  "tha-ap393:40.1_0": "‘Vinicchayaṁ pāpuṇāma,",
  "tha-ap393:40.2_0": "pucchissāma vinicchaye;",
  "tha-ap393:40.3_0": "Yathāsaṇṭhaṁ kathessanti,",
  "tha-ap393:40.4_0": "paṭipucchāma taṁ tathā’.",
  "tha-ap393:41.1_0": "Rañño hatthe gahetvāna,",
  "tha-ap393:41.2_0": "agamāsiṁ vinicchayaṁ;",
  "tha-ap393:41.3_0": "Purato akkhadassānaṁ,",
  "tha-ap393:41.4_0": "idaṁ vacanamabraviṁ.",
  "tha-ap393:42.1_0": "‘Suṇantu me akkhadassā,",
  "tha-ap393:42.2_0": "rājā varamadāsi me;",
  "tha-ap393:42.3_0": "Na kiñci ṭhapayitvāna,",
  "tha-ap393:42.4_0": "jīvitampi pavārayi.",
  "tha-ap393:43.1_0": "Tassa me varadinnassa,",
  "tha-ap393:43.2_0": "buddhaseṭṭhaṁ variṁ ahaṁ;",
  "tha-ap393:43.3_0": "Sudinno hoti me buddho,",
  "tha-ap393:43.4_0": "chindatha saṁsayaṁ mama’.",
  "tha-ap393:44.1_0": "‘Sossāma tava vacanaṁ,",
  "tha-ap393:44.2_0": "bhūmipālassa rājino;",
  "tha-ap393:44.3_0": "Ubhinnaṁ vacanaṁ sutvā,",
  "tha-ap393:44.4_0": "chindissāmettha saṁsayaṁ.",
  "tha-ap393:45.1_0": "Sabbaṁ deva tayā dinnaṁ,",
  "tha-ap393:45.2_0": "imassa sabbagāhikaṁ;",
  "tha-ap393:45.3_0": "Na kiñci ṭhapayitvāna,",
  "tha-ap393:45.4_0": "jīvitampi pavārayi’.",
  "tha-ap393:46.1_0": "‘Kicchappattova hutvāna,",
  "tha-ap393:46.2_0": "yācī varamanuttaraṁ;",
  "tha-ap393:46.3_0": "Imaṁ sudukkhitaṁ ñatvā,",
  "tha-ap393:46.4_0": "adāsiṁ sabbagāhikaṁ’.",
  "tha-ap393:47.1_0": "‘Parājayo tuvaṁ deva,",
  "tha-ap393:47.2_0": "assa deyyo tathāgato;",
  "tha-ap393:47.3_0": "Ubhinnaṁ saṁsayo chinno,",
  "tha-ap393:47.4_0": "yathāsaṇṭhamhi tiṭṭhatha’.",
  "tha-ap393:48.1_0": "Rājā tattheva ṭhatvāna,",
  "tha-ap393:48.2_0": "akkhadassetadabravi;",
  "tha-ap393:48.3_0": "‘Sammā mayhampi deyyātha,",
  "tha-ap393:48.4_0": "puna buddhaṁ labhāmahaṁ’.",
  "tha-ap393:49.1_0": "‘Pūretvā tava saṅkappaṁ,",
  "tha-ap393:49.2_0": "bhojayitvā tathāgataṁ;",
  "tha-ap393:49.3_0": "Puna deyyāsi sambuddhaṁ,",
  "tha-ap393:49.4_0": "ānandassa yasassino’.",
  "tha-ap393:50.0_0": "1.4. Nimantanakathā",
  "tha-ap393:50.1_0": "Akkhadassebhivādetvā,",
  "tha-ap393:50.2_0": "ānandañcāpi khattiyaṁ;",
  "tha-ap393:50.3_0": "Tuṭṭho pamudito hutvā,",
  "tha-ap393:50.4_0": "sambuddhamupasaṅkamiṁ.",
  "tha-ap393:51.1_0": "Upasaṅkamma sambuddhaṁ,",
  "tha-ap393:51.2_0": "oghatiṇṇamanāsavaṁ;",
  "tha-ap393:51.3_0": "Sirasā abhivādetvā,",
  "tha-ap393:51.4_0": "idaṁ vacanamabraviṁ.",
  "tha-ap393:52.1_0": "‘Vasīsatasahassehi,",
  "tha-ap393:52.2_0": "adhivāsehi cakkhuma;",
  "tha-ap393:52.3_0": "Hāsayanto mama cittaṁ,",
  "tha-ap393:52.4_0": "nivesanamupehi me’.",
  "tha-ap393:53.1_0": "Padumuttaro lokavidū,",
  "tha-ap393:53.2_0": "āhutīnaṁ paṭiggaho;",
  "tha-ap393:53.3_0": "Mama saṅkappamaññāya,",
  "tha-ap393:53.4_0": "adhivāsesi cakkhumā.",
  "tha-ap393:54.1_0": "Adhivāsanamaññāya,",
  "tha-ap393:54.2_0": "abhivādiya satthuno;",
  "tha-ap393:54.3_0": "Haṭṭho udaggacittohaṁ,",
  "tha-ap393:54.4_0": "nivesanamupāgamiṁ.",
  "tha-ap393:55.0_0": "1.5. Dānapaṭiyādana",
  "tha-ap393:55.1_0": "Mittāmacce samānetvā,",
  "tha-ap393:55.2_0": "idaṁ vacanamabraviṁ;",
  "tha-ap393:55.3_0": "‘Sudullabho mayā laddho,",
  "tha-ap393:55.4_0": "maṇi jotiraso yathā.",
  "tha-ap393:56.1_0": "Kena taṁ pūjayissāma,",
  "tha-ap393:56.2_0": "appameyyo anūpamo;",
  "tha-ap393:56.3_0": "Atulo asamo dhīro,",
  "tha-ap393:56.4_0": "jino appaṭipuggalo.",
  "tha-ap393:57.1_0": "Tathāsamasamo ceva,",
  "tha-ap393:57.2_0": "adutiyo narāsabho;",
  "tha-ap393:57.3_0": "Dukkaraṁ adhikārañhi,",
  "tha-ap393:57.4_0": "buddhānucchavikaṁ mayā.",
  "tha-ap393:58.1_0": "Nānāpupphe samānetvā,",
  "tha-ap393:58.2_0": "karoma pupphamaṇḍapaṁ;",
  "tha-ap393:58.3_0": "Buddhānucchavikaṁ etaṁ,",
  "tha-ap393:58.4_0": "sabbapūjā bhavissati’.",
  "tha-ap393:59.1_0": "Uppalaṁ padumaṁ vāpi,",
  "tha-ap393:59.2_0": "vassikaṁ adhimuttakaṁ;",
  "tha-ap393:59.3_0": "Campakaṁ nāgapupphañca,",
  "tha-ap393:59.4_0": "maṇḍapaṁ kārayiṁ ahaṁ.",
  "tha-ap393:60.1_0": "Satāsanasahassāni,",
  "tha-ap393:60.2_0": "chattacchāyāya paññapiṁ;",
  "tha-ap393:60.3_0": "Pacchimaṁ āsanaṁ mayhaṁ,",
  "tha-ap393:60.4_0": "adhikaṁ satamagghati.",
  "tha-ap393:61.1_0": "Satāsanasahassāni,",
  "tha-ap393:61.2_0": "chattacchāyāya paññapiṁ;",
  "tha-ap393:61.3_0": "Paṭiyādetvā annapānaṁ,",
  "tha-ap393:61.4_0": "kālaṁ ārocayiṁ ahaṁ.",
  "tha-ap393:62.1_0": "Ārocitamhi kālamhi,",
  "tha-ap393:62.2_0": "padumuttaro mahāmuni;",
  "tha-ap393:62.3_0": "Vasīsatasahassehi,",
  "tha-ap393:62.4_0": "nivesanamupesi me.",
  "tha-ap393:63.1_0": "Dhārentaṁ uparicchattaṁ,",
  "tha-ap393:63.2_0": "suphullapupphamaṇḍape;",
  "tha-ap393:63.3_0": "Vasīsatasahassehi,",
  "tha-ap393:63.4_0": "nisīdi purisuttamo.",
  "tha-ap393:64.1_0": "‘Chattasatasahassāni,",
  "tha-ap393:64.2_0": "satasahassamāsanaṁ;",
  "tha-ap393:64.3_0": "Kappiyaṁ anavajjañca,",
  "tha-ap393:64.4_0": "paṭigaṇhāhi cakkhuma’.",
  "tha-ap393:65.1_0": "Padumuttaro lokavidū,",
  "tha-ap393:65.2_0": "āhutīnaṁ paṭiggaho;",
  "tha-ap393:65.3_0": "Mamaṁ tāretukāmo so,",
  "tha-ap393:65.4_0": "sampaṭicchi mahāmuni.",
  "tha-ap393:66.0_0": "1.6. Dānakathā",
  "tha-ap393:66.1_0": "Bhikkhuno ekamekassa,",
  "tha-ap393:66.2_0": "Paccekaṁ pattamadāsahaṁ;",
  "tha-ap393:66.3_0": "Jahiṁsu sumbhakaṁ pattaṁ,",
  "tha-ap393:66.4_0": "Lohapattaṁ adhārayuṁ.",
  "tha-ap393:67.1_0": "Sattarattindivaṁ buddho,",
  "tha-ap393:67.2_0": "nisīdi pupphamaṇḍape;",
  "tha-ap393:67.3_0": "Bodhayanto bahū satte,",
  "tha-ap393:67.4_0": "dhammacakkaṁ pavattayi.",
  "tha-ap393:68.1_0": "Dhammacakkaṁ pavattento,",
  "tha-ap393:68.2_0": "heṭṭhato pupphamaṇḍape;",
  "tha-ap393:68.3_0": "Cullāsītisahassānaṁ,",
  "tha-ap393:68.4_0": "dhammābhisamayo ahu.",
  "tha-ap393:69.1_0": "Sattame divase patte,",
  "tha-ap393:69.2_0": "padumuttaro mahāmuni;",
  "tha-ap393:69.3_0": "Chattacchāyāyamāsīno,",
  "tha-ap393:69.4_0": "imā gāthā abhāsatha.",
  "tha-ap393:70.0_0": "1.7. Byākaraṇa",
  "tha-ap393:70.1_0": "‘Anūnakaṁ dānavaraṁ,",
  "tha-ap393:70.2_0": "yo me pādāsi māṇavo;",
  "tha-ap393:70.3_0": "Tamahaṁ kittayissāmi,",
  "tha-ap393:70.4_0": "suṇātha mama bhāsato.",
  "tha-ap393:71.1_0": "Hatthī assā rathā pattī,",
  "tha-ap393:71.2_0": "senā ca caturaṅginī;",
  "tha-ap393:71.3_0": "Parivāressantimaṁ niccaṁ,",
  "tha-ap393:71.4_0": "sabbadānassidaṁ phalaṁ.",
  "tha-ap393:72.1_0": "Hatthiyānaṁ assayānaṁ,",
  "tha-ap393:72.2_0": "sivikā sandamānikā;",
  "tha-ap393:72.3_0": "Upaṭṭhissantimaṁ niccaṁ,",
  "tha-ap393:72.4_0": "sabbadānassidaṁ phalaṁ.",
  "tha-ap393:73.1_0": "Saṭṭhi rathasahassāni,",
  "tha-ap393:73.2_0": "sabbālaṅkārabhūsitā;",
  "tha-ap393:73.3_0": "Parivāressantimaṁ niccaṁ,",
  "tha-ap393:73.4_0": "sabbadānassidaṁ phalaṁ.",
  "tha-ap393:74.1_0": "Saṭṭhi tūriyasahassāni,",
  "tha-ap393:74.2_0": "bheriyo samalaṅkatā;",
  "tha-ap393:74.3_0": "Vajjayissantimaṁ niccaṁ,",
  "tha-ap393:74.4_0": "sabbadānassidaṁ phalaṁ.",
  "tha-ap393:75.1_0": "Chaḷāsītisahassāni,",
  "tha-ap393:75.2_0": "nāriyo samalaṅkatā;",
  "tha-ap393:75.3_0": "Vicittavatthābharaṇā,",
  "tha-ap393:75.4_0": "āmukkamaṇikuṇḍalā.",
  "tha-ap393:76.1_0": "Aḷārapamhā hasulā,",
  "tha-ap393:76.2_0": "susaññā tanumajjhimā;",
  "tha-ap393:76.3_0": "Parivāressantimaṁ niccaṁ,",
  "tha-ap393:76.4_0": "sabbadānassidaṁ phalaṁ.",
  "tha-ap393:77.1_0": "Tiṁsakappasahassāni,",
  "tha-ap393:77.2_0": "devaloke ramissati;",
  "tha-ap393:77.3_0": "Sahassakkhattuṁ devindo,",
  "tha-ap393:77.4_0": "devarajjaṁ karissati.",
  "tha-ap393:78.1_0": "Sahassakkhattuṁ rājā ca,",
  "tha-ap393:78.2_0": "cakkavattī bhavissati;",
  "tha-ap393:78.3_0": "Padesarajjaṁ vipulaṁ,",
  "tha-ap393:78.4_0": "gaṇanāto asaṅkhiyaṁ.",
  "tha-ap393:79.1_0": "Devaloke vasantassa,",
  "tha-ap393:79.2_0": "puññakammasamaṅgino;",
  "tha-ap393:79.3_0": "Devalokapariyantaṁ,",
  "tha-ap393:79.4_0": "ratanachattaṁ dharissati.",
  "tha-ap393:80.1_0": "Icchissati yadā chāyaṁ,",
  "tha-ap393:80.2_0": "chadanaṁ dussapupphajaṁ;",
  "tha-ap393:80.3_0": "Imassa cittamaññāya,",
  "tha-ap393:80.4_0": "nibaddhaṁ chādayissati.",
  "tha-ap393:81.1_0": "Devalokā cavitvāna,",
  "tha-ap393:81.2_0": "sukkamūlena codito;",
  "tha-ap393:81.3_0": "Puññakammena saṁyutto,",
  "tha-ap393:81.4_0": "brahmabandhu bhavissati.",
  "tha-ap393:82.1_0": "Kappasatasahassamhi,",
  "tha-ap393:82.2_0": "okkākakulasambhavo;",
  "tha-ap393:82.3_0": "Gotamo nāma gottena,",
  "tha-ap393:82.4_0": "satthā loke bhavissati.",
  "tha-ap393:83.1_0": "Sabbametaṁ abhiññāya,",
  "tha-ap393:83.2_0": "gotamo sakyapuṅgavo;",
  "tha-ap393:83.3_0": "Bhikkhusaṅghe nisīditvā,",
  "tha-ap393:83.4_0": "etadagge ṭhapessati.",
  "tha-ap393:84.1_0": "Pilindavacchanāmena,",
  "tha-ap393:84.2_0": "hessati satthusāvako;",
  "tha-ap393:84.3_0": "Devānaṁ asurānañca,",
  "tha-ap393:84.4_0": "gandhabbānañca sakkato.",
  "tha-ap393:85.1_0": "Bhikkhūnaṁ bhikkhunīnañca,",
  "tha-ap393:85.2_0": "gihīnañca tatheva so;",
  "tha-ap393:85.3_0": "Piyo hutvāna sabbesaṁ,",
  "tha-ap393:85.4_0": "viharissatināsavo’.",
  "tha-ap393:86.0_0": "1.8. Dānānisaṁsakathā",
  "tha-ap393:86.1_0": "Satasahasse kataṁ kammaṁ,",
  "tha-ap393:86.2_0": "phalaṁ dassesi me idha;",
  "tha-ap393:86.3_0": "Sumutto saravegova,",
  "tha-ap393:86.4_0": "kilese jhāpayī mama.",
  "tha-ap393:87.1_0": "Aho me sukataṁ kammaṁ,",
  "tha-ap393:87.2_0": "puññakkhette anuttare;",
  "tha-ap393:87.3_0": "Yattha kāraṁ karitvāna,",
  "tha-ap393:87.4_0": "pattomhi acalaṁ padaṁ.",
  "tha-ap393:88.1_0": "Anūnakaṁ dānavaraṁ,",
  "tha-ap393:88.2_0": "adāsi yo hi māṇavo;",
  "tha-ap393:88.3_0": "Ādipubbaṅgamo āsi,",
  "tha-ap393:88.4_0": "tassa dānassidaṁ phalaṁ.",
  "tha-ap393:89.0_0": "1.8.1. Chattānisaṁsa",
  "tha-ap393:89.1_0": "Chatte ca sugate datvā,",
  "tha-ap393:89.2_0": "saṅghe gaṇavaruttame;",
  "tha-ap393:89.3_0": "Aṭṭhānisaṁse anubhomi,",
  "tha-ap393:89.4_0": "kammānucchavike mama.",
  "tha-ap393:90.1_0": "Sītaṁ uṇhaṁ na jānāmi,",
  "tha-ap393:90.2_0": "rajojallaṁ na limpati;",
  "tha-ap393:90.3_0": "Anupaddavo anīti ca,",
  "tha-ap393:90.4_0": "homi apacito sadā.",
  "tha-ap393:91.1_0": "Sukhumacchaviko homi,",
  "tha-ap393:91.2_0": "visadaṁ hoti mānasaṁ;",
  "tha-ap393:91.3_0": "Chattasatasahassāni,",
  "tha-ap393:91.4_0": "bhave saṁsarato mama.",
  "tha-ap393:92.1_0": "Sabbālaṅkārayuttāni,",
  "tha-ap393:92.2_0": "tassa kammassa vāhasā;",
  "tha-ap393:92.3_0": "Imaṁ jātiṁ ṭhapetvāna,",
  "tha-ap393:92.4_0": "matthake dhārayanti me.",
  "tha-ap393:93.1_0": "Kasmā imāya jātiyā,",
  "tha-ap393:93.2_0": "natthi me chattadhāraṇā;",
  "tha-ap393:93.3_0": "Mama sabbaṁ kataṁ kammaṁ,",
  "tha-ap393:93.4_0": "vimuttichattapattiyā.",
  "tha-ap393:94.0_0": "1.8.2. Dussānisaṁsa",
  "tha-ap393:94.1_0": "Dussāni sugate datvā,",
  "tha-ap393:94.2_0": "saṅghe gaṇavaruttame;",
  "tha-ap393:94.3_0": "Aṭṭhānisaṁse anubhomi,",
  "tha-ap393:94.4_0": "kammānucchavike mama.",
  "tha-ap393:95.1_0": "Suvaṇṇavaṇṇo virajo,",
  "tha-ap393:95.2_0": "sappabhāso patāpavā;",
  "tha-ap393:95.3_0": "Siniddhaṁ hoti me gattaṁ,",
  "tha-ap393:95.4_0": "bhave saṁsarato mama.",
  "tha-ap393:96.1_0": "Dussasatasahassāni,",
  "tha-ap393:96.2_0": "setā pītā ca lohitā;",
  "tha-ap393:96.3_0": "Dhārenti matthake mayhaṁ,",
  "tha-ap393:96.4_0": "dussadānassidaṁ phalaṁ.",
  "tha-ap393:97.1_0": "Koseyyakambaliyāni,",
  "tha-ap393:97.2_0": "khomakappāsikāni ca;",
  "tha-ap393:97.3_0": "Sabbattha paṭilabhāmi,",
  "tha-ap393:97.4_0": "tesaṁ nissandato ahaṁ.",
  "tha-ap393:98.0_0": "1.8.3. Pattānisaṁsa",
  "tha-ap393:98.1_0": "Patte sugate datvāna,",
  "tha-ap393:98.2_0": "saṅghe gaṇavaruttame;",
  "tha-ap393:98.3_0": "Dasānisaṁse anubhomi,",
  "tha-ap393:98.4_0": "kammānucchavike mama.",
  "tha-ap393:99.1_0": "Suvaṇṇathāle maṇithāle,",
  "tha-ap393:99.2_0": "rajatepi ca thālake;",
  "tha-ap393:99.3_0": "Lohitaṅgamaye thāle,",
  "tha-ap393:99.4_0": "paribhuñjāmi sabbadā.",
  "tha-ap393:100.1_0": "Anupaddavo anīti ca,",
  "tha-ap393:100.2_0": "homi apacito sadā;",
  "tha-ap393:100.3_0": "Lābhī annassa pānassa,",
  "tha-ap393:100.4_0": "vatthassa sayanassa ca.",
  "tha-ap393:101.1_0": "Na vinassanti me bhogā,",
  "tha-ap393:101.2_0": "ṭhitacitto bhavāmahaṁ;",
  "tha-ap393:101.3_0": "Dhammakāmo sadā homi,",
  "tha-ap393:101.4_0": "appakleso anāsavo.",
  "tha-ap393:102.1_0": "Devaloke manusse vā,",
  "tha-ap393:102.2_0": "anubandhā ime guṇā;",
  "tha-ap393:102.3_0": "Chāyā yathāpi rukkhassa,",
  "tha-ap393:102.4_0": "sabbattha na jahanti maṁ.",
  "tha-ap393:103.0_0": "1.8.4. Vāsiānisaṁsa",
  "tha-ap393:103.1_0": "Cittabandhanasambaddhā,",
  "tha-ap393:103.2_0": "sukatā vāsiyo bahū;",
  "tha-ap393:103.3_0": "Datvāna buddhaseṭṭhassa,",
  "tha-ap393:103.4_0": "saṅghassa ca tathevahaṁ.",
  "tha-ap393:104.1_0": "Aṭṭhānisaṁse anubhomi,",
  "tha-ap393:104.2_0": "kammānucchavike mama;",
  "tha-ap393:104.3_0": "Sūro homavisārī ca,",
  "tha-ap393:104.4_0": "vesārajjesu pāramī.",
  "tha-ap393:105.1_0": "Dhitivīriyavā homi,",
  "tha-ap393:105.2_0": "paggahītamano sadā;",
  "tha-ap393:105.3_0": "Kilesacchedanaṁ ñāṇaṁ,",
  "tha-ap393:105.4_0": "sukhumaṁ atulaṁ suciṁ;",
  "tha-ap393:105.5_0": "Sabbattha paṭilabhāmi,",
  "tha-ap393:105.6_0": "tassa nissandato ahaṁ.",
  "tha-ap393:106.0_0": "1.8.5. Satthakānisaṁsa",
  "tha-ap393:106.1_0": "Akakkase apharuse,",
  "tha-ap393:106.2_0": "sudhote satthake bahū;",
  "tha-ap393:106.3_0": "Pasannacitto datvāna,",
  "tha-ap393:106.4_0": "buddhe saṅghe tatheva ca.",
  "tha-ap393:107.1_0": "Pañcānisaṁse anubhomi,",
  "tha-ap393:107.2_0": "kammānucchavike mama;",
  "tha-ap393:107.3_0": "Kalyāṇamittaṁ vīriyaṁ,",
  "tha-ap393:107.4_0": "khantiñca mettasatthakaṁ.",
  "tha-ap393:108.1_0": "Taṇhāsallassa chinnattā,",
  "tha-ap393:108.2_0": "paññāsatthaṁ anuttaraṁ;",
  "tha-ap393:108.3_0": "Vajirena samaṁ ñāṇaṁ,",
  "tha-ap393:108.4_0": "tesaṁ nissandato labhe.",
  "tha-ap393:109.0_0": "1.8.6. Sūciānisaṁsa",
  "tha-ap393:109.1_0": "Sūciyo sugate datvā,",
  "tha-ap393:109.2_0": "saṅghe gaṇavaruttame;",
  "tha-ap393:109.3_0": "Pañcānisaṁse anubhomi,",
  "tha-ap393:109.4_0": "kammānucchavike mama.",
  "tha-ap393:110.1_0": "Na saṁsayo kaṅkhacchedo,",
  "tha-ap393:110.2_0": "abhirūpo ca bhogavā;",
  "tha-ap393:110.3_0": "Tikkhapañño sadā homi,",
  "tha-ap393:110.4_0": "saṁsaranto bhavābhave.",
  "tha-ap393:111.1_0": "Gambhīraṁ nipuṇaṁ ṭhānaṁ,",
  "tha-ap393:111.2_0": "atthaṁ ñāṇena passayiṁ;",
  "tha-ap393:111.3_0": "Vajiraggasamaṁ ñāṇaṁ,",
  "tha-ap393:111.4_0": "hoti me tamaghātanaṁ.",
  "tha-ap393:112.0_0": "1.8.7. Nakhacchedanānisaṁsa",
  "tha-ap393:112.1_0": "Nakhacchedane sugate,",
  "tha-ap393:112.2_0": "datvā saṅghe gaṇuttame;",
  "tha-ap393:112.3_0": "Pañcānisaṁse anubhomi,",
  "tha-ap393:112.4_0": "kammānucchavike mama.",
  "tha-ap393:113.1_0": "Dāsidāse gavasse ca,",
  "tha-ap393:113.2_0": "bhatake nāṭake bahū;",
  "tha-ap393:113.3_0": "Nhāpite bhattake sūde,",
  "tha-ap393:113.4_0": "sabbattheva labhāmahaṁ.",
  "tha-ap393:114.0_0": "1.8.8. Vidhūpanatālavaṇṭānisaṁsa",
  "tha-ap393:114.1_0": "Vidhūpane sugate datvā,",
  "tha-ap393:114.2_0": "tālavaṇṭe ca sobhane;",
  "tha-ap393:114.3_0": "Aṭṭhānisaṁse anubhomi,",
  "tha-ap393:114.4_0": "kammānucchavike mama.",
  "tha-ap393:115.1_0": "Sītaṁ uṇhaṁ na jānāmi,",
  "tha-ap393:115.2_0": "pariḷāho na vijjati;",
  "tha-ap393:115.3_0": "Darathaṁ nābhijānāmi,",
  "tha-ap393:115.4_0": "cittasantāpanaṁ mama.",
  "tha-ap393:116.1_0": "Rāgaggi dosamohaggi,",
  "tha-ap393:116.2_0": "mānaggi diṭṭhiaggi ca;",
  "tha-ap393:116.3_0": "Sabbaggī nibbutā mayhaṁ,",
  "tha-ap393:116.4_0": "tassa nissandato mama.",
  "tha-ap393:117.0_0": "1.8.9. Morahattha-cāmara",
  "tha-ap393:117.1_0": "Morahatthe cāmariyo,",
  "tha-ap393:117.2_0": "datvā saṅghe gaṇuttame;",
  "tha-ap393:117.3_0": "Upasantakilesohaṁ,",
  "tha-ap393:117.4_0": "viharāmi anaṅgaṇo.",
  "tha-ap393:118.0_0": "1.8.10. Parissāvana-dhammakara",
  "tha-ap393:118.1_0": "Parissāvane sugate,",
  "tha-ap393:118.2_0": "datvā dhammakaruttame;",
  "tha-ap393:118.3_0": "Pañcānisaṁse anubhomi,",
  "tha-ap393:118.4_0": "kammānucchavike mama.",
  "tha-ap393:119.1_0": "Sabbesaṁ samatikkamma,",
  "tha-ap393:119.2_0": "dibbaṁ āyuṁ labhāmahaṁ;",
  "tha-ap393:119.3_0": "Appasayho sadā homi,",
  "tha-ap393:119.4_0": "corapaccatthikehi vā.",
  "tha-ap393:120.1_0": "Satthena vā visena vā,",
  "tha-ap393:120.2_0": "vihesampi na kubbate;",
  "tha-ap393:120.3_0": "Antarāmaraṇaṁ natthi,",
  "tha-ap393:120.4_0": "tesaṁ nissandato mama.",
  "tha-ap393:121.0_0": "1.8.11. Teladhārānisaṁsa",
  "tha-ap393:121.1_0": "Teladhāre sugate datvā,",
  "tha-ap393:121.2_0": "saṅghe gaṇavaruttame;",
  "tha-ap393:121.3_0": "Pañcānisaṁse anubhomi,",
  "tha-ap393:121.4_0": "kammānucchavike mama.",
  "tha-ap393:122.1_0": "Sucārurūpo subhaddo,",
  "tha-ap393:122.2_0": "susamuggatamānaso;",
  "tha-ap393:122.3_0": "Avikkhittamano homi,",
  "tha-ap393:122.4_0": "sabbārakkhehi rakkhito.",
  "tha-ap393:123.0_0": "1.8.12. Sūcigharānisaṁsa",
  "tha-ap393:123.1_0": "Sūcighare sugate datvā,",
  "tha-ap393:123.2_0": "saṅghe gaṇavaruttame;",
  "tha-ap393:123.3_0": "Tīṇānisaṁse anubhomi,",
  "tha-ap393:123.4_0": "kammānucchavike mama.",
  "tha-ap393:124.1_0": "Cetosukhaṁ kāyasukhaṁ,",
  "tha-ap393:124.2_0": "iriyāpathajaṁ sukhaṁ;",
  "tha-ap393:124.3_0": "Ime guṇe paṭilabhe,",
  "tha-ap393:124.4_0": "tassa nissandato ahaṁ.",
  "tha-ap393:125.0_0": "1.8.13. Aṁsabaddhānisaṁsa",
  "tha-ap393:125.1_0": "Aṁsabaddhe jine datvā,",
  "tha-ap393:125.2_0": "saṅghe gaṇavaruttame;",
  "tha-ap393:125.3_0": "Tīṇānisaṁse anubhomi,",
  "tha-ap393:125.4_0": "kammānucchavike mama.",
  "tha-ap393:126.1_0": "Saddhamme gādhaṁ vindāmi,",
  "tha-ap393:126.2_0": "Sarāmi dutiyaṁ bhavaṁ;",
  "tha-ap393:126.3_0": "Sabbattha succhavī homi,",
  "tha-ap393:126.4_0": "Tassa nissandato ahaṁ.",
  "tha-ap393:127.0_0": "1.8.14. Kāyabandhanānisaṁsa",
  "tha-ap393:127.1_0": "Kāyabandhe jine datvā,",
  "tha-ap393:127.2_0": "saṅghe gaṇavaruttame;",
  "tha-ap393:127.3_0": "Chānisaṁse anubhomi,",
  "tha-ap393:127.4_0": "kammānucchavike mama.",
  "tha-ap393:128.1_0": "Samādhīsu na kampāmi,",
  "tha-ap393:128.2_0": "vasī homi samādhisu;",
  "tha-ap393:128.3_0": "Abhejjapariso homi,",
  "tha-ap393:128.4_0": "ādeyyavacano sadā.",
  "tha-ap393:129.1_0": "Upaṭṭhitasati homi,",
  "tha-ap393:129.2_0": "tāso mayhaṁ na vijjati;",
  "tha-ap393:129.3_0": "Devaloke manusse vā,",
  "tha-ap393:129.4_0": "anubandhā ime guṇā.",
  "tha-ap393:130.0_0": "1.8.15. Ādhārakānisaṁsa",
  "tha-ap393:130.1_0": "Ādhārake jine datvā,",
  "tha-ap393:130.2_0": "saṅghe gaṇavaruttame;",
  "tha-ap393:130.3_0": "Pañcavaṇṇehi dāyādo,",
  "tha-ap393:130.4_0": "acalo homi kenaci.",
  "tha-ap393:131.1_0": "Ye keci me sutā dhammā,",
  "tha-ap393:131.2_0": "satiñāṇappabodhanā;",
  "tha-ap393:131.3_0": "Dhatā me na vinassanti,",
  "tha-ap393:131.4_0": "bhavanti suvinicchitā.",
  "tha-ap393:132.0_0": "1.8.16. Bhājanānisaṁsa",
  "tha-ap393:132.1_0": "Bhājane paribhoge ca,",
  "tha-ap393:132.2_0": "datvā buddhe gaṇuttame;",
  "tha-ap393:132.3_0": "Tīṇānisaṁse anubhomi,",
  "tha-ap393:132.4_0": "kammānucchavike mama.",
  "tha-ap393:133.1_0": "Soṇṇamaye maṇimaye,",
  "tha-ap393:133.2_0": "athopi phalikāmaye;",
  "tha-ap393:133.3_0": "Lohitaṅgamaye ceva,",
  "tha-ap393:133.4_0": "labhāmi bhājane ahaṁ.",
  "tha-ap393:134.1_0": "Bhariyā dāsadāsī ca,",
  "tha-ap393:134.2_0": "hatthissarathapattike;",
  "tha-ap393:134.3_0": "Itthī patibbatā ceva,",
  "tha-ap393:134.4_0": "paribhogāni sabbadā.",
  "tha-ap393:135.1_0": "Vijjā mantapade ceva,",
  "tha-ap393:135.2_0": "vividhe āgame bahū;",
  "tha-ap393:135.3_0": "Sabbaṁ sippaṁ nisāmemi,",
  "tha-ap393:135.4_0": "paribhogāni sabbadā.",
  "tha-ap393:136.0_0": "1.8.17. Thālakānisaṁsa",
  "tha-ap393:136.1_0": "Thālake sugate datvā,",
  "tha-ap393:136.2_0": "saṅghe gaṇavaruttame;",
  "tha-ap393:136.3_0": "Tīṇānisaṁse anubhomi,",
  "tha-ap393:136.4_0": "kammānucchavike mama.",
  "tha-ap393:137.1_0": "Soṇṇamaye maṇimaye,",
  "tha-ap393:137.2_0": "athopi phalikāmaye;",
  "tha-ap393:137.3_0": "Lohitaṅgamaye ceva,",
  "tha-ap393:137.4_0": "labhāmi thālake ahaṁ.",
  "tha-ap393:138.1_0": "Asatthake phalamaye,",
  "tha-ap393:138.2_0": "atho pokkharapattake;",
  "tha-ap393:138.3_0": "Madhupānakasaṅkhe ca,",
  "tha-ap393:138.4_0": "labhāmi thālake ahaṁ.",
  "tha-ap393:139.1_0": "Vatte guṇe paṭipatti,",
  "tha-ap393:139.2_0": "ācārakiriyāsu ca;",
  "tha-ap393:139.3_0": "Ime guṇe paṭilabhe,",
  "tha-ap393:139.4_0": "tassa nissandato ahaṁ.",
  "tha-ap393:140.0_0": "1.8.18. Bhesajjānisaṁsa",
  "tha-ap393:140.1_0": "Bhesajjaṁ sugate datvā,",
  "tha-ap393:140.2_0": "saṅghe gaṇavaruttame;",
  "tha-ap393:140.3_0": "Dasānisaṁse anubhomi,",
  "tha-ap393:140.4_0": "kammānucchavike mama.",
  "tha-ap393:141.1_0": "Āyuvā balavā dhīro,",
  "tha-ap393:141.2_0": "vaṇṇavā yasavā sukhī;",
  "tha-ap393:141.3_0": "Anupaddavo anīti ca,",
  "tha-ap393:141.4_0": "homi apacito sadā;",
  "tha-ap393:141.5_0": "Na me piyaviyogatthi,",
  "tha-ap393:141.6_0": "tassa nissandato mama.",
  "tha-ap393:142.0_0": "1.8.19. Upāhanānisaṁsa",
  "tha-ap393:142.1_0": "Upāhane jine datvā,",
  "tha-ap393:142.2_0": "saṅghe gaṇavaruttame;",
  "tha-ap393:142.3_0": "Tīṇānisaṁse anubhomi,",
  "tha-ap393:142.4_0": "kammānucchavike mama.",
  "tha-ap393:143.1_0": "Hatthiyānaṁ assayānaṁ,",
  "tha-ap393:143.2_0": "sivikā sandamānikā;",
  "tha-ap393:143.3_0": "Saṭṭhisatasahassāni,",
  "tha-ap393:143.4_0": "parivārenti maṁ sadā.",
  "tha-ap393:144.1_0": "Maṇimayā tambamayā,",
  "tha-ap393:144.2_0": "soṇṇarajatapādukā;",
  "tha-ap393:144.3_0": "Nibbattanti paduddhāre,",
  "tha-ap393:144.4_0": "bhave saṁsarato mama.",
  "tha-ap393:145.1_0": "Niyāmaṁ sati dhāvanti,",
  "tha-ap393:145.2_0": "āguācārasodhanaṁ;",
  "tha-ap393:145.3_0": "Ime guṇe paṭilabhe,",
  "tha-ap393:145.4_0": "tassa nissandato ahaṁ.",
  "tha-ap393:146.0_0": "1.8.20. Pādukānisaṁsa",
  "tha-ap393:146.1_0": "Pāduke sugate datvā,",
  "tha-ap393:146.2_0": "saṅghe gaṇavaruttame;",
  "tha-ap393:146.3_0": "Iddhipādukamāruyha,",
  "tha-ap393:146.4_0": "viharāmi yadicchakaṁ.",
  "tha-ap393:147.0_0": "1.8.21. Udakapuñchanānisaṁsa",
  "tha-ap393:147.1_0": "Udakapuñchanacoḷe,",
  "tha-ap393:147.2_0": "datvā buddhe gaṇuttame;",
  "tha-ap393:147.3_0": "Pañcānisaṁse anubhomi,",
  "tha-ap393:147.4_0": "kammānucchavike mama.",
  "tha-ap393:148.1_0": "Suvaṇṇavaṇṇo virajo,",
  "tha-ap393:148.2_0": "sappabhāso patāpavā;",
  "tha-ap393:148.3_0": "Siniddhaṁ hoti me gattaṁ,",
  "tha-ap393:148.4_0": "rajojallaṁ na limpati;",
  "tha-ap393:148.5_0": "Ime guṇe paṭilabhe,",
  "tha-ap393:148.6_0": "tassa nissandato ahaṁ.",
  "tha-ap393:149.0_0": "1.8.22. Kattaradaṇḍānisaṁsa",
  "tha-ap393:149.1_0": "Kattaradaṇḍe sugate,",
  "tha-ap393:149.2_0": "datvā saṅghe gaṇuttame;",
  "tha-ap393:149.3_0": "Chānisaṁse anubhomi,",
  "tha-ap393:149.4_0": "kammānucchavike mama.",
  "tha-ap393:150.1_0": "Puttā mayhaṁ bahū honti,",
  "tha-ap393:150.2_0": "tāso mayhaṁ na vijjati;",
  "tha-ap393:150.3_0": "Appasayho sadā homi,",
  "tha-ap393:150.4_0": "sabbārakkhehi rakkhito;",
  "tha-ap393:150.5_0": "Khalitampi na jānāmi,",
  "tha-ap393:150.6_0": "abhantaṁ mānasaṁ mama.",
  "tha-ap393:151.0_0": "1.8.23. Osadhañjanānisaṁsa",
  "tha-ap393:151.1_0": "Osadhaṁ añjanaṁ datvā,",
  "tha-ap393:151.2_0": "buddhe saṅghe gaṇuttame;",
  "tha-ap393:151.3_0": "Aṭṭhānisaṁse anubhomi,",
  "tha-ap393:151.4_0": "kammānucchavike mama.",
  "tha-ap393:152.1_0": "Visālanayano homi,",
  "tha-ap393:152.2_0": "setapīto ca lohito;",
  "tha-ap393:152.3_0": "Anāvilapasannakkho,",
  "tha-ap393:152.4_0": "sabbarogavivajjito.",
  "tha-ap393:153.1_0": "Labhāmi dibbanayanaṁ,",
  "tha-ap393:153.2_0": "paññācakkhuṁ anuttaraṁ;",
  "tha-ap393:153.3_0": "Ime guṇe paṭilabhe,",
  "tha-ap393:153.4_0": "tassa nissandato ahaṁ.",
  "tha-ap393:154.0_0": "1.8.24. Kuñcikānisaṁsa",
  "tha-ap393:154.1_0": "Kuñcike sugate datvā,",
  "tha-ap393:154.2_0": "saṅghe gaṇavaruttame;",
  "tha-ap393:154.3_0": "Dhammadvāravivaraṇaṁ,",
  "tha-ap393:154.4_0": "labhāmi ñāṇakuñcikaṁ.",
  "tha-ap393:155.0_0": "1.8.25. Kuñcikāgharānisaṁsa",
  "tha-ap393:155.1_0": "Kuñcikānaṁ ghare datvā,",
  "tha-ap393:155.2_0": "buddhe saṅghe gaṇuttame;",
  "tha-ap393:155.3_0": "Dvānisaṁse anubhomi,",
  "tha-ap393:155.4_0": "kammānucchavike mama;",
  "tha-ap393:155.5_0": "Appakodho anāyāso,",
  "tha-ap393:155.6_0": "saṁsaranto bhave ahaṁ.",
  "tha-ap393:156.0_0": "1.8.26. Āyogānisaṁsa",
  "tha-ap393:156.1_0": "Āyoge sugate datvā,",
  "tha-ap393:156.2_0": "saṅghe gaṇavaruttame;",
  "tha-ap393:156.3_0": "Pañcānisaṁse anubhomi,",
  "tha-ap393:156.4_0": "kammānucchavike mama.",
  "tha-ap393:157.1_0": "Samādhīsu na kampāmi,",
  "tha-ap393:157.2_0": "vasī homi samādhisu;",
  "tha-ap393:157.3_0": "Abhejjapariso homi,",
  "tha-ap393:157.4_0": "ādeyyavacano sadā;",
  "tha-ap393:157.5_0": "Jāyati bhogasampatti,",
  "tha-ap393:157.6_0": "bhave saṁsarato mama.",
  "tha-ap393:158.0_0": "1.8.27. Dhūmanettānisaṁsa",
  "tha-ap393:158.1_0": "Dhūmanette jine datvā,",
  "tha-ap393:158.2_0": "saṅghe gaṇavaruttame;",
  "tha-ap393:158.3_0": "Tīṇānisaṁse anubhomi,",
  "tha-ap393:158.4_0": "kammānucchavike mama.",
  "tha-ap393:159.1_0": "Sati me ujukā hoti,",
  "tha-ap393:159.2_0": "susambandhā ca nhāravo;",
  "tha-ap393:159.3_0": "Labhāmi dibbanayanaṁ,",
  "tha-ap393:159.4_0": "tassa nissandato ahaṁ.",
  "tha-ap393:160.0_0": "1.8.28. Dīpadhārānisaṁsa",
  "tha-ap393:160.1_0": "Dīpadhāre jine datvā,",
  "tha-ap393:160.2_0": "saṅghe gaṇavaruttame;",
  "tha-ap393:160.3_0": "Tīṇānisaṁse anubhomi,",
  "tha-ap393:160.4_0": "kammānucchavike mama.",
  "tha-ap393:161.1_0": "Jātimā aṅgasampanno,",
  "tha-ap393:161.2_0": "paññavā buddhasammato;",
  "tha-ap393:161.3_0": "Ime guṇe paṭilabhe,",
  "tha-ap393:161.4_0": "tassa nissandato ahaṁ.",
  "tha-ap393:162.0_0": "1.8.29. Tumbaka-karaṇḍa",
  "tha-ap393:162.1_0": "Tumbake ca karaṇḍe ca,",
  "tha-ap393:162.2_0": "datvā buddhe gaṇuttame;",
  "tha-ap393:162.3_0": "Dasānisaṁse anubhomi,",
  "tha-ap393:162.4_0": "kammānucchavike mama.",
  "tha-ap393:163.1_0": "Sugutto sukhasamaṅgī,",
  "tha-ap393:163.2_0": "Mahāyaso tathāgati;",
  "tha-ap393:163.3_0": "Vipattivigato sukhumālo,",
  "tha-ap393:163.4_0": "Sabbītiparivajjito.",
  "tha-ap393:164.1_0": "Vipule ca guṇe lābhī,",
  "tha-ap393:164.2_0": "samāva calanā mama;",
  "tha-ap393:164.3_0": "Suvivajjitaubbego,",
  "tha-ap393:164.4_0": "tumbake ca karaṇḍake.",
  "tha-ap393:165.1_0": "Labhāmi caturo vaṇṇe,",
  "tha-ap393:165.2_0": "hatthissaratanāni ca;",
  "tha-ap393:165.3_0": "Tāni me na vinassanti,",
  "tha-ap393:165.4_0": "tumbadāne idaṁ phalaṁ.",
  "tha-ap393:166.0_0": "1.8.30. Malaharaṇānisaṁsa",
  "tha-ap393:166.1_0": "Malaharaṇiyo datvā,",
  "tha-ap393:166.2_0": "buddhe saṅghe gaṇuttame;",
  "tha-ap393:166.3_0": "Pañcānisaṁse anubhomi,",
  "tha-ap393:166.4_0": "kammānucchavike mama.",
  "tha-ap393:167.1_0": "Sabbalakkhaṇasampanno,",
  "tha-ap393:167.2_0": "āyupaññāsamāhito;",
  "tha-ap393:167.3_0": "Sabbāyāsavinimutto,",
  "tha-ap393:167.4_0": "kāyo me hoti sabbadā.",
  "tha-ap393:168.0_0": "1.8.31. Pipphalānisaṁsa",
  "tha-ap393:168.1_0": "Taṇudhāre sunisite,",
  "tha-ap393:168.2_0": "saṅghe datvāna pipphale;",
  "tha-ap393:168.3_0": "Kilesakantanaṁ ñāṇaṁ,",
  "tha-ap393:168.4_0": "labhāmi atulaṁ suciṁ.",
  "tha-ap393:169.0_0": "1.8.32. Saṇḍāsānisaṁsa",
  "tha-ap393:169.1_0": "Saṇḍāse sugate datvā,",
  "tha-ap393:169.2_0": "saṅghe gaṇavaruttame;",
  "tha-ap393:169.3_0": "Kilesabhañjanaṁ ñāṇaṁ,",
  "tha-ap393:169.4_0": "labhāmi atulaṁ suciṁ.",
  "tha-ap393:170.0_0": "1.8.33. Natthukānisaṁsa",
  "tha-ap393:170.1_0": "Natthuke sugate datvā,",
  "tha-ap393:170.2_0": "saṅghe gaṇavaruttame;",
  "tha-ap393:170.3_0": "Aṭṭhānisaṁse anubhomi,",
  "tha-ap393:170.4_0": "kammānucchavike mama.",
  "tha-ap393:171.1_0": "Saddhaṁ sīlaṁ hiriñcāpi,",
  "tha-ap393:171.2_0": "atha ottappiyaṁ guṇaṁ;",
  "tha-ap393:171.3_0": "Sutaṁ cāgañca khantiñca,",
  "tha-ap393:171.4_0": "paññaṁ me aṭṭhamaṁ guṇaṁ.",
  "tha-ap393:172.0_0": "1.8.34. Pīṭhakānisaṁsa",
  "tha-ap393:172.1_0": "Pīṭhake sugate datvā,",
  "tha-ap393:172.2_0": "saṅghe gaṇavaruttame;",
  "tha-ap393:172.3_0": "Pañcānisaṁse anubhomi,",
  "tha-ap393:172.4_0": "kammānucchavike mama.",
  "tha-ap393:173.1_0": "Ucce kule pajāyāmi,",
  "tha-ap393:173.2_0": "mahābhogo bhavāmahaṁ;",
  "tha-ap393:173.3_0": "Sabbe maṁ apacāyanti,",
  "tha-ap393:173.4_0": "kitti abbhuggatā mama.",
  "tha-ap393:174.1_0": "Kappasatasahassāni,",
  "tha-ap393:174.2_0": "pallaṅkā caturassakā;",
  "tha-ap393:174.3_0": "Parivārenti maṁ niccaṁ,",
  "tha-ap393:174.4_0": "saṁvibhāgarato ahaṁ.",
  "tha-ap393:175.0_0": "1.8.35. Bhisiānisaṁsa",
  "tha-ap393:175.1_0": "Bhisiyo sugate datvā,",
  "tha-ap393:175.2_0": "saṅghe gaṇavaruttame;",
  "tha-ap393:175.3_0": "Chānisaṁse anubhomi,",
  "tha-ap393:175.4_0": "kammānucchavike mama.",
  "tha-ap393:176.1_0": "Samasugattopacito,",
  "tha-ap393:176.2_0": "muduko cārudassano;",
  "tha-ap393:176.3_0": "Labhāmi ñāṇaparivāraṁ,",
  "tha-ap393:176.4_0": "bhisidānassidaṁ phalaṁ.",
  "tha-ap393:177.1_0": "Tūlikā vikatikāyo,",
  "tha-ap393:177.2_0": "kaṭṭissā cittakā bahū;",
  "tha-ap393:177.3_0": "Varapotthake kambale ca,",
  "tha-ap393:177.4_0": "labhāmi vividhe ahaṁ.",
  "tha-ap393:178.1_0": "Pāvārike ca muduke,",
  "tha-ap393:178.2_0": "mudukājinaveṇiyo;",
  "tha-ap393:178.3_0": "Labhāmi vividhatthāre,",
  "tha-ap393:178.4_0": "bhisidānassidaṁ phalaṁ.",
  "tha-ap393:179.1_0": "Yato sarāmi attānaṁ,",
  "tha-ap393:179.2_0": "yato pattosmi viññutaṁ;",
  "tha-ap393:179.3_0": "Atuccho jhānamañcomhi,",
  "tha-ap393:179.4_0": "bhisidānassidaṁ phalaṁ.",
  "tha-ap393:180.0_0": "1.8.36. Bibbohanānisaṁsa",
  "tha-ap393:180.1_0": "Bibbohane jine datvā,",
  "tha-ap393:180.2_0": "saṅghe gaṇavaruttame;",
  "tha-ap393:180.3_0": "Chānisaṁse anubhomi,",
  "tha-ap393:180.4_0": "kammānucchavike mama.",
  "tha-ap393:181.1_0": "Uṇṇike padumake ca,",
  "tha-ap393:181.2_0": "atho lohitacandane;",
  "tha-ap393:181.3_0": "Bibbohane upādhemi,",
  "tha-ap393:181.4_0": "uttamaṅgaṁ sadā mama.",
  "tha-ap393:182.1_0": "Aṭṭhaṅgike maggavare,",
  "tha-ap393:182.2_0": "sāmaññe caturo phale;",
  "tha-ap393:182.3_0": "Tesu ñāṇaṁ uppādetvā,",
  "tha-ap393:182.4_0": "vihare niccakālikaṁ.",
  "tha-ap393:183.1_0": "Dāne dame saṁyame ca,",
  "tha-ap393:183.2_0": "appamaññāsu rūpisu;",
  "tha-ap393:183.3_0": "Tesu ñāṇaṁ uppādetvā,",
  "tha-ap393:183.4_0": "vihare sabbakālikaṁ.",
  "tha-ap393:184.1_0": "Vatte guṇe paṭipatti,",
  "tha-ap393:184.2_0": "ācārakiriyāsu ca;",
  "tha-ap393:184.3_0": "Tesu ñāṇaṁ uppādetvā,",
  "tha-ap393:184.4_0": "vihare sabbadā ahaṁ.",
  "tha-ap393:185.1_0": "Caṅkame vā padhāne vā,",
  "tha-ap393:185.2_0": "vīriye bodhipakkhiye;",
  "tha-ap393:185.3_0": "Tesu ñāṇaṁ uppādetvā,",
  "tha-ap393:185.4_0": "viharāmi yadicchakaṁ.",
  "tha-ap393:186.1_0": "Sīlaṁ samādhi paññā ca,",
  "tha-ap393:186.2_0": "vimutti ca anuttarā;",
  "tha-ap393:186.3_0": "Tesu ñāṇaṁ uppādetvā,",
  "tha-ap393:186.4_0": "viharāmi sukhaṁ ahaṁ.",
  "tha-ap393:187.0_0": "1.8.37. Phalapīṭhānisaṁsa",
  "tha-ap393:187.1_0": "Phalapīṭhe jine datvā,",
  "tha-ap393:187.2_0": "saṅghe gaṇavaruttame;",
  "tha-ap393:187.3_0": "Dvānisaṁse anubhomi,",
  "tha-ap393:187.4_0": "kammānucchavike mama.",
  "tha-ap393:188.1_0": "Soṇṇamaye maṇimaye,",
  "tha-ap393:188.2_0": "dantasāramaye bahū;",
  "tha-ap393:188.3_0": "Pallaṅkaseṭṭhe vindāmi,",
  "tha-ap393:188.4_0": "phalapīṭhassidaṁ phalaṁ.",
  "tha-ap393:189.0_0": "1.8.38. Pādapīṭhānisaṁsa",
  "tha-ap393:189.1_0": "Pādapīṭhe jine datvā,",
  "tha-ap393:189.2_0": "saṅghe gaṇavaruttame;",
  "tha-ap393:189.3_0": "Dvānisaṁse anubhomi,",
  "tha-ap393:189.4_0": "kammānucchavike mama;",
  "tha-ap393:189.5_0": "Labhāmi bahuke yāne,",
  "tha-ap393:189.6_0": "pādapīṭhassidaṁ phalaṁ.",
  "tha-ap393:190.1_0": "Dāsī dāsā ca bhariyā,",
  "tha-ap393:190.2_0": "ye caññe anujīvino;",
  "tha-ap393:190.3_0": "Sammā paricarante maṁ,",
  "tha-ap393:190.4_0": "pādapīṭhassidaṁ phalaṁ.",
  "tha-ap393:191.0_0": "1.8.39. Telabbhañjanānisaṁsa",
  "tha-ap393:191.1_0": "Telaabbhañjane datvā,",
  "tha-ap393:191.2_0": "saṅghe gaṇavaruttame;",
  "tha-ap393:191.3_0": "Pañcānisaṁse anubhomi,",
  "tha-ap393:191.4_0": "kammānucchavike mama.",
  "tha-ap393:192.1_0": "Abyādhitā rūpavatā,",
  "tha-ap393:192.2_0": "khippaṁ dhammanisantitā;",
  "tha-ap393:192.3_0": "Lābhitā annapānassa,",
  "tha-ap393:192.4_0": "āyupañcamakaṁ mama.",
  "tha-ap393:193.0_0": "1.8.40. Sappitelānisaṁsa",
  "tha-ap393:193.1_0": "Sappitelañca datvāna,",
  "tha-ap393:193.2_0": "saṅghe gaṇavaruttame;",
  "tha-ap393:193.3_0": "Pañcānisaṁse anubhomi,",
  "tha-ap393:193.4_0": "kammānucchavike mama.",
  "tha-ap393:194.1_0": "Thāmavā rūpasampanno,",
  "tha-ap393:194.2_0": "pahaṭṭhatanujo sadā;",
  "tha-ap393:194.3_0": "Abyādhi visado homi,",
  "tha-ap393:194.4_0": "sappitelassidaṁ phalaṁ.",
  "tha-ap393:195.0_0": "1.8.41. Mukhasodhanakānisaṁsa",
  "tha-ap393:195.1_0": "Mukhasodhanakaṁ datvā,",
  "tha-ap393:195.2_0": "buddhe saṅghe gaṇuttame;",
  "tha-ap393:195.3_0": "Pañcānisaṁse anubhomi,",
  "tha-ap393:195.4_0": "kammānucchavike mama.",
  "tha-ap393:196.1_0": "Visuddhakaṇṭho madhurassaro,",
  "tha-ap393:196.2_0": "Kāsasāsavivajjito;",
  "tha-ap393:196.3_0": "Uppalagandho mukhato,",
  "tha-ap393:196.4_0": "Upavāyati me sadā.",
  "tha-ap393:197.0_0": "1.8.42. Dadhiānisaṁsa",
  "tha-ap393:197.1_0": "Dadhiṁ datvāna sampannaṁ,",
  "tha-ap393:197.2_0": "buddhe saṅghe gaṇuttame;",
  "tha-ap393:197.3_0": "Bhuñjāmi amataṁ bhattaṁ,",
  "tha-ap393:197.4_0": "varaṁ kāyagatāsatiṁ.",
  "tha-ap393:198.0_0": "1.8.43. Madhuānisaṁsa",
  "tha-ap393:198.1_0": "Vaṇṇagandharasopetaṁ,",
  "tha-ap393:198.2_0": "madhuṁ datvā jine gaṇe;",
  "tha-ap393:198.3_0": "Anūpamaṁ atuliyaṁ,",
  "tha-ap393:198.4_0": "pive muttirasaṁ ahaṁ.",
  "tha-ap393:199.0_0": "1.8.44. Rasānisaṁsa",
  "tha-ap393:199.1_0": "Yathābhūtaṁ rasaṁ datvā,",
  "tha-ap393:199.2_0": "buddhe saṅghe gaṇuttame;",
  "tha-ap393:199.3_0": "Caturo phale anubhomi,",
  "tha-ap393:199.4_0": "kammānucchavike mama.",
  "tha-ap393:200.0_0": "1.8.45. Annapānānisaṁsa",
  "tha-ap393:200.1_0": "Annaṁ pānañca datvāna,",
  "tha-ap393:200.2_0": "buddhe saṅghe gaṇuttame;",
  "tha-ap393:200.3_0": "Dasānisaṁse anubhomi,",
  "tha-ap393:200.4_0": "kammānucchavike mama.",
  "tha-ap393:201.1_0": "Āyuvā balavā dhīro,",
  "tha-ap393:201.2_0": "vaṇṇavā yasavā sukhī;",
  "tha-ap393:201.3_0": "Lābhī annassa pānassa,",
  "tha-ap393:201.4_0": "sūro paññāṇavā sadā;",
  "tha-ap393:201.5_0": "Ime guṇe paṭilabhe,",
  "tha-ap393:201.6_0": "saṁsaranto bhave ahaṁ.",
  "tha-ap393:202.0_0": "1.8.46. Dhūpānisaṁsa",
  "tha-ap393:202.1_0": "Dhūpaṁ datvāna sugate,",
  "tha-ap393:202.2_0": "saṅghe gaṇavaruttame;",
  "tha-ap393:202.3_0": "Dasānisaṁse anubhomi,",
  "tha-ap393:202.4_0": "kammānucchavike mama.",
  "tha-ap393:203.1_0": "Sugandhadeho yasavā,",
  "tha-ap393:203.2_0": "sīghapañño ca kittimā;",
  "tha-ap393:203.3_0": "Tikkhapañño bhūripañño,",
  "tha-ap393:203.4_0": "hāsagambhīrapaññavā.",
  "tha-ap393:204.1_0": "Vepullajavanapañño,",
  "tha-ap393:204.2_0": "saṁsaranto bhavābhave;",
  "tha-ap393:204.3_0": "Tasseva vāhasā dāni,",
  "tha-ap393:204.4_0": "patto santisukhaṁ sivaṁ.",
  "tha-ap393:205.0_0": "1.9. Sādhāraṇānisaṁsa",
  "tha-ap393:205.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap393:205.2_0": "bhavā sabbe samūhatā;",
  "tha-ap393:205.3_0": "Nāgova bandhanaṁ chetvā,",
  "tha-ap393:205.4_0": "viharāmi anāsavo.",
  "tha-ap393:206.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap393:206.2_0": "mama buddhassa santike;",
  "tha-ap393:206.3_0": "Tisso vijjā anuppattā,",
  "tha-ap393:206.4_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap393:207.1_0": "Paṭisambhidā catasso,",
  "tha-ap393:207.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap393:207.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap393:207.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap393:208.1_0": "Itthaṁ sudaṁ āyasmā pilindavaccho thero imā gāthāyo abhāsitthāti.",
  "tha-ap393:209.1_0": "Pilindavacchattherassāpadānaṁ paṭhamaṁ."
}