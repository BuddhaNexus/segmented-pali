{
  "pv11:0.1_0": "Petavatthu",
  "pv11:0.2_0": "Uragavagga",
  "pv11:0.3_0": "11. Nāgapetavatthu",
  "pv11:1.1_0": "“Puratova setena paleti hatthinā,",
  "pv11:1.2_0": "Majjhe pana assatarīrathena;",
  "pv11:1.3_0": "Pacchā ca kaññā sivikāya nīyati,",
  "pv11:1.4_0": "Obhāsayantī dasa sabbaso disā.",
  "pv11:2.1_0": "Tumhe pana muggarahatthapāṇino,",
  "pv11:2.2_0": "Rudammukhā chinnapabhinnagattā;",
  "pv11:2.3_0": "Manussabhūtā kimakattha pāpaṁ,",
  "pv11:2.4_0": "Yenaññamaññassa pivātha lohitan”ti.",
  "pv11:3.1_0": "“Puratova yo gacchati kuñjarena,",
  "pv11:3.2_0": "Setena nāgena catukkamena;",
  "pv11:3.3_0": "Amhāka putto ahu jeṭṭhako so,",
  "pv11:3.4_0": "Dānāni datvāna sukhī pamodati.",
  "pv11:4.1_0": "Yo so majjhe assatarīrathena,",
  "pv11:4.2_0": "Catubbhi yuttena suvaggitena;",
  "pv11:4.3_0": "Amhāka putto ahu majjhimo so,",
  "pv11:4.4_0": "Amaccharī dānavatī virocati.",
  "pv11:5.1_0": "Yā sā ca pacchā sivikāya nīyati,",
  "pv11:5.2_0": "Nārī sapaññā migamandalocanā;",
  "pv11:5.3_0": "Amhāka dhītā ahu sā kaniṭṭhikā,",
  "pv11:5.4_0": "Bhāgaḍḍhabhāgena sukhī pamodati.",
  "pv11:6.1_0": "Ete ca dānāni adaṁsu pubbe,",
  "pv11:6.2_0": "Pasannacittā samaṇabrāhmaṇānaṁ;",
  "pv11:6.3_0": "Mayaṁ pana maccharino ahumha,",
  "pv11:6.4_0": "Paribhāsakā samaṇabrāhmaṇānaṁ;",
  "pv11:6.5_0": "Ete ca datvā paricārayanti,",
  "pv11:6.6_0": "Mayañca sussāma naḷova chinno”ti.",
  "pv11:7.1_0": "“Kiṁ tumhākaṁ bhojanaṁ kiṁ sayānaṁ,",
  "pv11:7.2_0": "Kathañca yāpetha supāpadhammino;",
  "pv11:7.3_0": "Pahūtabhogesu anappakesu,",
  "pv11:7.4_0": "Sukhaṁ virādhāya dukkhajja pattā”ti.",
  "pv11:8.1_0": "“Aññamaññaṁ vadhitvāna,",
  "pv11:8.2_0": "pivāma pubbalohitaṁ;",
  "pv11:8.3_0": "Bahuṁ pitvā na dhātā homa,",
  "pv11:8.4_0": "nacchādimhase mayaṁ.",
  "pv11:9.1_0": "Icceva maccā paridevayanti,",
  "pv11:9.2_0": "Adāyakā pecca yamassa ṭhāyino;",
  "pv11:9.3_0": "Ye te vidicca adhigamma bhoge,",
  "pv11:9.4_0": "Na bhuñjare nāpi karonti puññaṁ.",
  "pv11:10.1_0": "Te khuppipāsūpagatā parattha,",
  "pv11:10.2_0": "Pacchā ciraṁ jhāyare ḍayhamānā;",
  "pv11:10.3_0": "Kammāni katvāna dukhudrāni,",
  "pv11:10.4_0": "Anubhonti dukkhaṁ kaṭukapphalāni.",
  "pv11:11.1_0": "Ittaraṁ hi dhanaṁ dhaññaṁ,",
  "pv11:11.2_0": "ittaraṁ idha jīvitaṁ;",
  "pv11:11.3_0": "Ittaraṁ ittarato ñatvā,",
  "pv11:11.4_0": "dīpaṁ kayirātha paṇḍito.",
  "pv11:12.1_0": "Ye te evaṁ pajānanti,",
  "pv11:12.2_0": "narā dhammassa kovidā;",
  "pv11:12.3_0": "Te dāne nappamajjanti,",
  "pv11:12.4_0": "sutvā arahataṁ vaco”ti.",
  "pv11:13.1_0": "Nāgapetavatthu ekādasamaṁ."
}