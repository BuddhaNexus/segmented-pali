{
  "ja501:0.1_0": "Jātaka",
  "ja501:0.2_0": "Vīsatinipāta",
  "ja501:0.3_0": "Mātaṅgavagga",
  "ja501:0.4_0": "5. Rohaṇamigajātaka",
  "ja501:1.1_0": "“Ete yūthā patiyanti,",
  "ja501:1.2_0": "bhītā maraṇassa cittaka;",
  "ja501:1.3_0": "Gaccha tuvampi mākaṅkhi,",
  "ja501:1.4_0": "jīvissanti tayā saha”.",
  "ja501:2.1_0": "“Nāhaṁ rohaṇa gacchāmi,",
  "ja501:2.2_0": "hadayaṁ me avakassati;",
  "ja501:2.3_0": "Na taṁ ahaṁ jahissāmi,",
  "ja501:2.4_0": "idha hissāmi jīvitaṁ”.",
  "ja501:3.1_0": "“Te hi nūna marissanti,",
  "ja501:3.2_0": "andhā apariṇāyakā;",
  "ja501:3.3_0": "Gaccha tuvampi mākaṅkhi,",
  "ja501:3.4_0": "jīvissanti tayā saha”.",
  "ja501:4.1_0": "“Nāhaṁ rohaṇa gacchāmi,",
  "ja501:4.2_0": "hadayaṁ me avakassati;",
  "ja501:4.3_0": "Na taṁ baddhaṁ jahissāmi,",
  "ja501:4.4_0": "idha hissāmi jīvitaṁ”.",
  "ja501:5.1_0": "“Gaccha bhīru palāyassu,",
  "ja501:5.2_0": "kūṭe baddhosmi āyase;",
  "ja501:5.3_0": "Gaccha tuvampi mākaṅkhi,",
  "ja501:5.4_0": "jīvissanti tayā saha”.",
  "ja501:6.1_0": "“Nāhaṁ rohaṇa gacchāmi,",
  "ja501:6.2_0": "hadayaṁ me avakassati;",
  "ja501:6.3_0": "Na taṁ ahaṁ jahissāmi,",
  "ja501:6.4_0": "idha hissāmi jīvitaṁ”.",
  "ja501:7.1_0": "“Te hi nūna marissanti,",
  "ja501:7.2_0": "andhā apariṇāyakā;",
  "ja501:7.3_0": "Gaccha tuvampi mākaṅkhi,",
  "ja501:7.4_0": "jīvissanti tayā saha”.",
  "ja501:8.1_0": "“Nāhaṁ rohaṇa gacchāmi,",
  "ja501:8.2_0": "hadayaṁ me avakassati;",
  "ja501:8.3_0": "Na taṁ baddhaṁ jahissāmi,",
  "ja501:8.4_0": "idha hissāmi jīvitaṁ”.",
  "ja501:9.1_0": "“Ayaṁ so luddako eti,",
  "ja501:9.2_0": "luddarūpo sahāvudho;",
  "ja501:9.3_0": "Yo no vadhissati ajja,",
  "ja501:9.4_0": "usunā sattiyā api”.",
  "ja501:10.1_0": "“Sā muhuttaṁ palāyitvā,",
  "ja501:10.2_0": "bhayaṭṭā bhayatajjitā;",
  "ja501:10.3_0": "Sudukkaraṁ akarā bhīru,",
  "ja501:10.4_0": "maraṇāyūpanivattatha”.",
  "ja501:11.1_0": "“Kiṁ nu teme migā honti,",
  "ja501:11.2_0": "muttā baddhaṁ upāsare;",
  "ja501:11.3_0": "Na taṁ cajitumicchanti,",
  "ja501:11.4_0": "jīvitassapi kāraṇā”.",
  "ja501:12.1_0": "“Bhātaro honti me ludda,",
  "ja501:12.2_0": "sodariyā ekamātukā;",
  "ja501:12.3_0": "Na maṁ cajitumicchanti,",
  "ja501:12.4_0": "jīvitassapi kāraṇā”.",
  "ja501:13.1_0": "“Te hi nūna marissanti,",
  "ja501:13.2_0": "andhā apariṇāyakā;",
  "ja501:13.3_0": "Pañcannaṁ jīvitaṁ dehi,",
  "ja501:13.4_0": "bhātaraṁ muñca luddaka”.",
  "ja501:14.1_0": "“So vo ahaṁ pamokkhāmi,",
  "ja501:14.2_0": "mātāpettibharaṁ migaṁ;",
  "ja501:14.3_0": "Nandantu mātāpitaro,",
  "ja501:14.4_0": "muttaṁ disvā mahāmigaṁ”.",
  "ja501:15.1_0": "“Evaṁ luddaka nandassu,",
  "ja501:15.2_0": "saha sabbehi ñātibhi;",
  "ja501:15.3_0": "Yathāhamajja nandāmi,",
  "ja501:15.4_0": "muttaṁ disvā mahāmigaṁ”.",
  "ja501:16.1_0": "“Kathaṁ tvaṁ pamokkho āsi,",
  "ja501:16.2_0": "Upanītasmi jīvite;",
  "ja501:16.3_0": "Kathaṁ putta amocesi,",
  "ja501:16.4_0": "Kūṭapāsamha luddako”.",
  "ja501:17.1_0": "“Bhaṇaṁ kaṇṇasukhaṁ vācaṁ,",
  "ja501:17.2_0": "hadayaṅgaṁ hadayassitaṁ;",
  "ja501:17.3_0": "Subhāsitāhi vācāhi,",
  "ja501:17.4_0": "cittako maṁ amocayi.",
  "ja501:18.1_0": "Bhaṇaṁ kaṇṇasukhaṁ vācaṁ,",
  "ja501:18.2_0": "hadayaṅgaṁ hadayassitaṁ;",
  "ja501:18.3_0": "Subhāsitāhi vācāhi,",
  "ja501:18.4_0": "sutanā maṁ amocayi.",
  "ja501:19.1_0": "Sutvā kaṇṇasukhaṁ vācaṁ,",
  "ja501:19.2_0": "hadayaṅgaṁ hadayassitaṁ;",
  "ja501:19.3_0": "Subhāsitāni sutvāna,",
  "ja501:19.4_0": "luddako maṁ amocayi”.",
  "ja501:20.1_0": "“Evaṁ ānandito hotu,",
  "ja501:20.2_0": "saha dārehi luddako;",
  "ja501:20.3_0": "Yathā mayajja nandāma,",
  "ja501:20.4_0": "disvā rohaṇamāgataṁ”.",
  "ja501:21.1_0": "“Nanu tvaṁ avaca ludda,",
  "ja501:21.2_0": "‘migacammāni āhariṁ’;",
  "ja501:21.3_0": "Atha kena nu vaṇṇena,",
  "ja501:21.4_0": "migacammāni nāhari”.",
  "ja501:22.1_0": "“Āgamā ceva hatthatthaṁ,",
  "ja501:22.2_0": "kūṭapāsañca so migo;",
  "ja501:22.3_0": "Abajjhi taṁ migarājaṁ,",
  "ja501:22.4_0": "tañca muttā upāsare.",
  "ja501:23.1_0": "Tassa me ahu saṁvego,",
  "ja501:23.2_0": "abbhuto lomahaṁsano;",
  "ja501:23.3_0": "Imañcāhaṁ migaṁ haññe,",
  "ja501:23.4_0": "ajja hissāmi jīvitaṁ”.",
  "ja501:24.1_0": "“Kīdisā te migā ludda,",
  "ja501:24.2_0": "kīdisā dhammikā migā;",
  "ja501:24.3_0": "Kathaṁvaṇṇā kathaṁsīlā,",
  "ja501:24.4_0": "bāḷhaṁ kho ne pasaṁsasi”.",
  "ja501:25.1_0": "“Odātasiṅgā sucivālā,",
  "ja501:25.2_0": "jātarūpatacūpamā;",
  "ja501:25.3_0": "Pādā lohitakā tesaṁ,",
  "ja501:25.4_0": "añjitakkhā manoramā”.",
  "ja501:26.1_0": "“Edisā te migā deva,",
  "ja501:26.2_0": "edisā dhammikā migā;",
  "ja501:26.3_0": "Mātāpettibharā deva,",
  "ja501:26.4_0": "na te so abhihārituṁ”.",
  "ja501:27.1_0": "“Dammi nikkhasataṁ ludda,",
  "ja501:27.2_0": "thūlañca maṇikuṇḍalaṁ;",
  "ja501:27.3_0": "Catussadañca pallaṅkaṁ,",
  "ja501:27.4_0": "umāpupphasarinnibhaṁ.",
  "ja501:28.1_0": "Dve ca sādisiyo bhariyā,",
  "ja501:28.2_0": "usabhañca gavaṁ sataṁ;",
  "ja501:28.3_0": "Dhammena rajjaṁ kāressaṁ,",
  "ja501:28.4_0": "bahukāro mesi luddaka.",
  "ja501:29.1_0": "Kasivāṇijjā iṇadānaṁ,",
  "ja501:29.2_0": "uñchācariyā ca luddaka;",
  "ja501:29.3_0": "Etena dāraṁ posehi,",
  "ja501:29.4_0": "mā pāpaṁ akarī punāti”.",
  "ja501:30.1_0": "Rohaṇamigajātakaṁ pañcamaṁ."
}