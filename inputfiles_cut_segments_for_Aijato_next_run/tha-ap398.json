{
  "tha-ap398:0.1_0": "Therāpadāna",
  "tha-ap398:0.2_0": "Pilindavacchavagga",
  "tha-ap398:0.3_0": "6 Bākulattheraapadāna",
  "tha-ap398:1.1_0": "“Himavantassāvidūre,",
  "tha-ap398:1.2_0": "sobhito nāma pabbato;",
  "tha-ap398:1.3_0": "Assamo sukato mayhaṁ,",
  "tha-ap398:1.4_0": "sakasissehi māpito.",
  "tha-ap398:2.1_0": "Maṇḍapā ca bahū tattha,",
  "tha-ap398:2.2_0": "pupphitā sinduvārakā;",
  "tha-ap398:2.3_0": "Kapitthā ca bahū tattha,",
  "tha-ap398:2.4_0": "pupphitā jīvajīvakā.",
  "tha-ap398:3.1_0": "Nigguṇḍiyo bahū tattha,",
  "tha-ap398:3.2_0": "badarāmalakāni ca;",
  "tha-ap398:3.3_0": "Phārusakā alābū ca,",
  "tha-ap398:3.4_0": "puṇḍarīkā ca pupphitā.",
  "tha-ap398:4.1_0": "Āḷakā beluvā tattha,",
  "tha-ap398:4.2_0": "kadalī mātuluṅgakā;",
  "tha-ap398:4.3_0": "Mahānāmā bahū tattha,",
  "tha-ap398:4.4_0": "ajjunā ca piyaṅgukā.",
  "tha-ap398:5.1_0": "Kosambā saḷalā nimbā,",
  "tha-ap398:5.2_0": "nigrodhā ca kapitthanā;",
  "tha-ap398:5.3_0": "Ediso assamo mayhaṁ,",
  "tha-ap398:5.4_0": "sasissohaṁ tahiṁ vasiṁ.",
  "tha-ap398:6.1_0": "Anomadassī bhagavā,",
  "tha-ap398:6.2_0": "sayambhū lokanāyako;",
  "tha-ap398:6.3_0": "Gavesaṁ paṭisallānaṁ,",
  "tha-ap398:6.4_0": "mamassamamupāgami.",
  "tha-ap398:7.1_0": "Upetamhi mahāvīre,",
  "tha-ap398:7.2_0": "anomadassimahāyase;",
  "tha-ap398:7.3_0": "Khaṇena lokanāthassa,",
  "tha-ap398:7.4_0": "vātābādho samuṭṭhahi.",
  "tha-ap398:8.1_0": "Vicaranto araññamhi,",
  "tha-ap398:8.2_0": "addasaṁ lokanāyakaṁ;",
  "tha-ap398:8.3_0": "Upagantvāna sambuddhaṁ,",
  "tha-ap398:8.4_0": "cakkhumantaṁ mahāyasaṁ.",
  "tha-ap398:9.1_0": "Iriyañcāpi disvāna,",
  "tha-ap398:9.2_0": "upalakkhesahaṁ tadā;",
  "tha-ap398:9.3_0": "Asaṁsayañhi buddhassa,",
  "tha-ap398:9.4_0": "byādhi no udapajjatha.",
  "tha-ap398:10.1_0": "Khippaṁ assamamāgañchiṁ,",
  "tha-ap398:10.2_0": "mama sissāna santike;",
  "tha-ap398:10.3_0": "Bhesajjaṁ kattukāmohaṁ,",
  "tha-ap398:10.4_0": "sisse āmantayiṁ tadā.",
  "tha-ap398:11.1_0": "Paṭissuṇitvāna me vākyaṁ,",
  "tha-ap398:11.2_0": "sissā sabbe sagāravā;",
  "tha-ap398:11.3_0": "Ekajjhaṁ sannipatiṁsu,",
  "tha-ap398:11.4_0": "satthugāravatā mama.",
  "tha-ap398:12.1_0": "Khippaṁ pabbatamāruyha,",
  "tha-ap398:12.2_0": "sabbosadhamahāsahaṁ;",
  "tha-ap398:12.3_0": "Pānīyayogaṁ katvāna,",
  "tha-ap398:12.4_0": "buddhaseṭṭhassadāsahaṁ.",
  "tha-ap398:13.1_0": "Paribhutte mahāvīre,",
  "tha-ap398:13.2_0": "sabbaññulokanāyake;",
  "tha-ap398:13.3_0": "Khippaṁ vāto vūpasami,",
  "tha-ap398:13.4_0": "sugatassa mahesino.",
  "tha-ap398:14.1_0": "Passaddhaṁ darathaṁ disvā,",
  "tha-ap398:14.2_0": "anomadassī mahāyaso;",
  "tha-ap398:14.3_0": "Sakāsane nisīditvā,",
  "tha-ap398:14.4_0": "imā gāthā abhāsatha.",
  "tha-ap398:15.1_0": "‘Yo me pādāsi bhesajjaṁ,",
  "tha-ap398:15.2_0": "byādhiñca samayī mama;",
  "tha-ap398:15.3_0": "Tamahaṁ kittayissāmi,",
  "tha-ap398:15.4_0": "suṇātha mama bhāsato.",
  "tha-ap398:16.1_0": "Kappasatasahassāni,",
  "tha-ap398:16.2_0": "devaloke ramissati;",
  "tha-ap398:16.3_0": "Vādite tūriye tattha,",
  "tha-ap398:16.4_0": "modissati sadā ayaṁ.",
  "tha-ap398:17.1_0": "Manussalokamāgantvā,",
  "tha-ap398:17.2_0": "sukkamūlena codito;",
  "tha-ap398:17.3_0": "Sahassakkhattuṁ rājā ca,",
  "tha-ap398:17.4_0": "cakkavattī bhavissati.",
  "tha-ap398:18.1_0": "Pañcapaññāsakappamhi,",
  "tha-ap398:18.2_0": "anomo nāma khattiyo;",
  "tha-ap398:18.3_0": "Cāturanto vijitāvī,",
  "tha-ap398:18.4_0": "jambumaṇḍassa issaro.",
  "tha-ap398:19.1_0": "Sattaratanasampanno,",
  "tha-ap398:19.2_0": "cakkavattī mahabbalo;",
  "tha-ap398:19.3_0": "Tāvatiṁsepi khobhetvā,",
  "tha-ap398:19.4_0": "issaraṁ kārayissati.",
  "tha-ap398:20.1_0": "Devabhūto manusso vā,",
  "tha-ap398:20.2_0": "appābādho bhavissati;",
  "tha-ap398:20.3_0": "Pariggahaṁ vivajjetvā,",
  "tha-ap398:20.4_0": "byādhiṁ loke tarissati.",
  "tha-ap398:21.1_0": "Apparimeyye ito kappe,",
  "tha-ap398:21.2_0": "okkākakulasambhavo;",
  "tha-ap398:21.3_0": "Gotamo nāma gottena,",
  "tha-ap398:21.4_0": "satthā loke bhavissati.",
  "tha-ap398:22.1_0": "Tassa dhammesu dāyādo,",
  "tha-ap398:22.2_0": "oraso dhammanimmito;",
  "tha-ap398:22.3_0": "Sabbāsave pariññāya,",
  "tha-ap398:22.4_0": "nibbāyissatināsavo.",
  "tha-ap398:23.1_0": "Kilese jhāpayitvāna,",
  "tha-ap398:23.2_0": "taṇhāsotaṁ tarissati;",
  "tha-ap398:23.3_0": "Bākulo nāma nāmena,",
  "tha-ap398:23.4_0": "hessati satthu sāvako.",
  "tha-ap398:24.1_0": "Idaṁ sabbaṁ abhiññāya,",
  "tha-ap398:24.2_0": "gotamo sakyapuṅgavo;",
  "tha-ap398:24.3_0": "Bhikkhusaṅghe nisīditvā,",
  "tha-ap398:24.4_0": "etadagge ṭhapessati’.",
  "tha-ap398:25.1_0": "Anomadassī bhagavā,",
  "tha-ap398:25.2_0": "sayambhū lokanāyako;",
  "tha-ap398:25.3_0": "Vivekānuvilokento,",
  "tha-ap398:25.4_0": "mamassamamupāgami.",
  "tha-ap398:26.1_0": "Upāgataṁ mahāvīraṁ,",
  "tha-ap398:26.2_0": "sabbaññuṁ lokanāyakaṁ;",
  "tha-ap398:26.3_0": "Sabbosadhena tappesiṁ,",
  "tha-ap398:26.4_0": "pasanno sehi pāṇibhi.",
  "tha-ap398:27.1_0": "Tassa me sukataṁ kammaṁ,",
  "tha-ap398:27.2_0": "sukhette bījasampadā;",
  "tha-ap398:27.3_0": "Khepetuṁ neva sakkomi,",
  "tha-ap398:27.4_0": "tadā hi sukataṁ mama.",
  "tha-ap398:28.1_0": "Lābhā mama suladdhaṁ me,",
  "tha-ap398:28.2_0": "yohaṁ addakkhi nāyakaṁ;",
  "tha-ap398:28.3_0": "Tena kammāvasesena,",
  "tha-ap398:28.4_0": "pattomhi acalaṁ padaṁ.",
  "tha-ap398:29.1_0": "Sabbametaṁ abhiññāya,",
  "tha-ap398:29.2_0": "gotamo sakyapuṅgavo;",
  "tha-ap398:29.3_0": "Bhikkhusaṅghe nisīditvā,",
  "tha-ap398:29.4_0": "etadagge ṭhapesi maṁ.",
  "tha-ap398:30.1_0": "Aparimeyye ito kappe,",
  "tha-ap398:30.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap398:30.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap398:30.4_0": "bhesajjassa idaṁ phalaṁ.",
  "tha-ap398:31.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap398:31.2_0": "bhavā sabbe samūhatā;",
  "tha-ap398:31.3_0": "Sabbāsavā parikkhīṇā,",
  "tha-ap398:31.4_0": "natthi dāni punabbhavo.",
  "tha-ap398:32.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap398:32.2_0": "…pe…",
  "tha-ap398:32.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap398:33.1_0": "Paṭisambhidā catasso,",
  "tha-ap398:33.2_0": "…pe…",
  "tha-ap398:33.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap398:34.1_0": "Itthaṁ sudaṁ āyasmā bākulo thero imā gāthāyo abhāsitthāti.",
  "tha-ap398:35.1_0": "Bākulattherassāpadānaṁ chaṭṭhaṁ."
}