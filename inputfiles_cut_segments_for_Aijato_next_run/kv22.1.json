{
  "kv22.1:0.1_0": "Kathāvatthu",
  "kv22.1:0.2_0": "Khuddakaaḍḍhapaṇṇāsaka",
  "kv22.1:0.3_0": "Bāvīsatimavagga",
  "kv22.1:0.4_0": "Parinibbānakathā",
  "kv22.1:1.1_0": "Atthi kiñci saṁyojanaṁ appahāya parinibbānanti?",
  "kv22.1:1.2_0": "Āmantā.",
  "kv22.1:1.3_0": "Atthi kiñci sakkāyadiṭṭhiṁ appahāya …pe… anottappaṁ appahāya parinibbānanti?",
  "kv22.1:1.4_0": "Na hevaṁ vattabbe …pe….",
  "kv22.1:2.1_0": "Atthi kiñci saṁyojanaṁ appahāya parinibbānanti?",
  "kv22.1:2.2_0": "Āmantā.",
  "kv22.1:2.3_0": "Arahā sarāgo …pe… sakilesoti?",
  "kv22.1:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv22.1:2.5_0": "nanu arahā nirāgo …pe… nikkilesoti?",
  "kv22.1:2.6_0": "Āmantā.",
  "kv22.1:2.7_0": "Hañci arahā nirāgo …pe… nikkileso, no ca vata re vattabbe—",
  "kv22.1:2.8_0": "“atthi kiñci saṁyojanaṁ appahāya parinibbānan”ti.",
  "kv22.1:3.1_0": "Na vattabbaṁ—",
  "kv22.1:3.2_0": "“atthi kiñci saṁyojanaṁ appahāya parinibbānan”ti?",
  "kv22.1:3.3_0": "Āmantā.",
  "kv22.1:3.4_0": "Arahā sabbaṁ buddhavisayaṁ jānātīti?",
  "kv22.1:3.5_0": "Na hevaṁ vattabbe.",
  "kv22.1:3.6_0": "Tena hi atthi kiñci saṁyojanaṁ appahāya parinibbānanti.",
  "kv22.1:3.7_0": "Parinibbānakathā niṭṭhitā."
}