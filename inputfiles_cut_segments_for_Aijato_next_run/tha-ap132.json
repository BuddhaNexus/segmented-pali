{
  "tha-ap132:0.1_0": "Therāpadāna",
  "tha-ap132:0.2_0": "Sereyyavagga",
  "tha-ap132:0.3_0": "10. Padumapūjakattheraapadāna",
  "tha-ap132:1.1_0": "“Himavantassāvidūre,",
  "tha-ap132:1.2_0": "gotamo nāma pabbato;",
  "tha-ap132:1.3_0": "Nānārukkhehi sañchanno,",
  "tha-ap132:1.4_0": "mahābhūtagaṇālayo.",
  "tha-ap132:2.1_0": "Vemajjhamhi ca tassāsi,",
  "tha-ap132:2.2_0": "assamo abhinimmito;",
  "tha-ap132:2.3_0": "Purakkhato sasissehi,",
  "tha-ap132:2.4_0": "vasāmi assame ahaṁ.",
  "tha-ap132:3.1_0": "Āyantu me sissagaṇā,",
  "tha-ap132:3.2_0": "padumaṁ āharantu me;",
  "tha-ap132:3.3_0": "Buddhapūjaṁ karissāmi,",
  "tha-ap132:3.4_0": "dvipadindassa tādino.",
  "tha-ap132:4.1_0": "Evanti te paṭissutvā,",
  "tha-ap132:4.2_0": "padumaṁ āhariṁsu me;",
  "tha-ap132:4.3_0": "Tathā nimittaṁ katvāhaṁ,",
  "tha-ap132:4.4_0": "buddhassa abhiropayiṁ.",
  "tha-ap132:5.1_0": "Sisse tadā samānetvā,",
  "tha-ap132:5.2_0": "sādhukaṁ anusāsahaṁ;",
  "tha-ap132:5.3_0": "Mā kho tumhe pamajjittha,",
  "tha-ap132:5.4_0": "appamādo sukhāvaho.",
  "tha-ap132:6.1_0": "Evaṁ samanusāsitvā,",
  "tha-ap132:6.2_0": "te sisse vacanakkhame;",
  "tha-ap132:6.3_0": "Appamādaguṇe yutto,",
  "tha-ap132:6.4_0": "tadā kālaṅkato ahaṁ.",
  "tha-ap132:7.1_0": "Ekanavutito kappe,",
  "tha-ap132:7.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap132:7.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap132:7.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap132:8.1_0": "Ekapaññāsakappamhi,",
  "tha-ap132:8.2_0": "rājā āsiṁ jaluttamo;",
  "tha-ap132:8.3_0": "Sattaratanasampanno,",
  "tha-ap132:8.4_0": "cakkavattī mahabbalo.",
  "tha-ap132:9.1_0": "Paṭisambhidā catasso,",
  "tha-ap132:9.2_0": "…pe…",
  "tha-ap132:9.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap132:10.1_0": "Itthaṁ sudaṁ āyasmā padumapūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap132:11.1_0": "Padumapūjakattherassāpadānaṁ dasamaṁ.",
  "tha-ap132:12.1_0": "Sereyyavaggo terasamo.",
  "tha-ap132:13.1_0": "Tassuddānaṁ",
  "tha-ap132:14.1_0": "Sereyyako pupphathūpi,",
  "tha-ap132:14.2_0": "pāyaso gandhathomako;",
  "tha-ap132:14.3_0": "Āsani phalasaññī ca,",
  "tha-ap132:14.4_0": "gaṇṭhipadumapupphiyo;",
  "tha-ap132:14.5_0": "Pañcuttarasatā gāthā,",
  "tha-ap132:14.6_0": "gaṇitā atthadassibhi."
}