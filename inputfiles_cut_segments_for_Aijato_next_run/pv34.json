{
  "pv34:0.1_0": "Petavatthu",
  "pv34:0.2_0": "Cūḷavagga",
  "pv34:0.3_0": "9. Kūṭavinicchayikapetavatthu",
  "pv34:1.1_0": "“Mālī kiriṭī kāyūrī,",
  "pv34:1.2_0": "gattā te candanussadā;",
  "pv34:1.3_0": "Pasannamukhavaṇṇosi,",
  "pv34:1.4_0": "sūriyavaṇṇova sobhasi.",
  "pv34:2.1_0": "Amānusā pārisajjā,",
  "pv34:2.2_0": "ye teme paricārakā;",
  "pv34:2.3_0": "Dasa kaññāsahassāni,",
  "pv34:2.4_0": "yā temā paricārikā;",
  "pv34:2.5_0": "Tā kambukāyūradharā,",
  "pv34:2.6_0": "kañcanāveḷabhūsitā.",
  "pv34:3.1_0": "Mahānubhāvosi tuvaṁ,",
  "pv34:3.2_0": "lomahaṁsanarūpavā;",
  "pv34:3.3_0": "Piṭṭhimaṁsāni attano,",
  "pv34:3.4_0": "sāmaṁ ukkacca khādasi.",
  "pv34:4.1_0": "Kiṁ nu kāyena vācāya,",
  "pv34:4.2_0": "manasā dukkuṭaṁ kataṁ;",
  "pv34:4.3_0": "Kissa kammavipākena,",
  "pv34:4.4_0": "piṭṭhimaṁsāni attano;",
  "pv34:4.5_0": "Sāmaṁ ukkacca khādasī”ti.",
  "pv34:5.1_0": "“Attanohaṁ anatthāya,",
  "pv34:5.2_0": "jīvaloke acārisaṁ;",
  "pv34:5.3_0": "Pesuññamusāvādena,",
  "pv34:5.4_0": "nikativañcanāya ca.",
  "pv34:6.1_0": "Tatthāhaṁ parisaṁ gantvā,",
  "pv34:6.2_0": "Saccakāle upaṭṭhite;",
  "pv34:6.3_0": "Atthaṁ dhammaṁ nirākatvā,",
  "pv34:6.4_0": "Adhammamanuvattisaṁ.",
  "pv34:7.1_0": "Evaṁ so khādatattānaṁ,",
  "pv34:7.2_0": "yo hoti piṭṭhimaṁsiko;",
  "pv34:7.3_0": "Yathāhaṁ ajja khādāmi,",
  "pv34:7.4_0": "piṭṭhimaṁsāni attano.",
  "pv34:8.1_0": "Tayidaṁ tayā nārada sāmaṁ diṭṭhaṁ,",
  "pv34:8.2_0": "Anukampakā ye kusalā vadeyyuṁ;",
  "pv34:8.3_0": "Mā pesuṇaṁ mā ca musā abhāṇi,",
  "pv34:8.4_0": "Mā khosi piṭṭhimaṁsiko tuvan”ti.",
  "pv34:9.1_0": "Kūṭavinicchayikapetavatthu navamaṁ."
}