{
  "bv13:0.1_0": "Buddhavaṁsa",
  "bv13:0.2_0": "13. Sumedhabuddhavaṁsa",
  "bv13:1.1_0": "“Padumuttarassa aparena,",
  "bv13:1.2_0": "Sumedho nāma nāyako;",
  "bv13:1.3_0": "Durāsado uggatejo,",
  "bv13:1.4_0": "Sabbalokuttamo muni.",
  "bv13:2.1_0": "Pasannanetto sumukho,",
  "bv13:2.2_0": "brahā uju patāpavā;",
  "bv13:2.3_0": "Hitesī sabbasattānaṁ,",
  "bv13:2.4_0": "bahū mocesi bandhanā.",
  "bv13:3.1_0": "Yadā buddho pāpuṇitvā,",
  "bv13:3.2_0": "kevalaṁ bodhimuttamaṁ;",
  "bv13:3.3_0": "Sudassanamhi nagare,",
  "bv13:3.4_0": "dhammacakkaṁ pavattayi.",
  "bv13:4.1_0": "Tassāpi abhisamayā tīṇi,",
  "bv13:4.2_0": "Ahesuṁ dhammadesane;",
  "bv13:4.3_0": "Koṭisatasahassānaṁ,",
  "bv13:4.4_0": "Paṭhamābhisamayo ahu.",
  "bv13:5.1_0": "Punāparaṁ kumbhakaṇṇaṁ,",
  "bv13:5.2_0": "yakkhaṁ so damayī jino;",
  "bv13:5.3_0": "Navutikoṭisahassānaṁ,",
  "bv13:5.4_0": "dutiyābhisamayo ahu.",
  "bv13:6.1_0": "Punāparaṁ amitayaso,",
  "bv13:6.2_0": "catusaccaṁ pakāsayi;",
  "bv13:6.3_0": "Asītikoṭisahassānaṁ,",
  "bv13:6.4_0": "tatiyābhisamayo ahu.",
  "bv13:7.1_0": "Sannipātā tayo āsuṁ,",
  "bv13:7.2_0": "sumedhassa mahesino;",
  "bv13:7.3_0": "Khīṇāsavānaṁ vimalānaṁ,",
  "bv13:7.4_0": "santacittāna tādinaṁ.",
  "bv13:8.1_0": "Sudassanaṁ nāma nagaraṁ,",
  "bv13:8.2_0": "upagañchi jino yadā;",
  "bv13:8.3_0": "Tadā khīṇāsavā bhikkhū,",
  "bv13:8.4_0": "samiṁsu satakoṭiyo.",
  "bv13:9.1_0": "Punāparaṁ devakūṭe,",
  "bv13:9.2_0": "bhikkhūnaṁ kathinatthate;",
  "bv13:9.3_0": "Tadā navutikoṭīnaṁ,",
  "bv13:9.4_0": "dutiyo āsi samāgamo.",
  "bv13:10.1_0": "Punāparaṁ dasabalo,",
  "bv13:10.2_0": "yadā carati cārikaṁ;",
  "bv13:10.3_0": "Tadā asītikoṭīnaṁ,",
  "bv13:10.4_0": "tatiyo āsi samāgamo.",
  "bv13:11.1_0": "Ahaṁ tena samayena,",
  "bv13:11.2_0": "uttaro nāma māṇavo;",
  "bv13:11.3_0": "Asītikoṭiyo mayhaṁ,",
  "bv13:11.4_0": "ghare sannicitaṁ dhanaṁ.",
  "bv13:12.1_0": "Kevalaṁ sabbaṁ datvāna,",
  "bv13:12.2_0": "sasaṅghe lokanāyake;",
  "bv13:12.3_0": "Saraṇaṁ tassupagañchiṁ,",
  "bv13:12.4_0": "pabbajjañcābhirocayiṁ.",
  "bv13:13.1_0": "Sopi maṁ buddho byākāsi,",
  "bv13:13.2_0": "karonto anumodanaṁ;",
  "bv13:13.3_0": "‘Tiṁsakappasahassamhi,",
  "bv13:13.4_0": "ayaṁ buddho bhavissati.",
  "bv13:14.1_0": "Padhānaṁ padahitvāna,",
  "bv13:14.2_0": "…pe…",
  "bv13:14.3_0": "hessāma sammukhā imaṁ’.",
  "bv13:15.1_0": "Tassāpi vacanaṁ sutvā,",
  "bv13:15.2_0": "bhiyyo cittaṁ pasādayiṁ;",
  "bv13:15.3_0": "Uttariṁ vatamadhiṭṭhāsiṁ,",
  "bv13:15.4_0": "dasapāramipūriyā.",
  "bv13:16.1_0": "Suttantaṁ vinayañcāpi,",
  "bv13:16.2_0": "navaṅgaṁ satthusāsanaṁ;",
  "bv13:16.3_0": "Sabbaṁ pariyāpuṇitvāna,",
  "bv13:16.4_0": "sobhayiṁ jinasāsanaṁ.",
  "bv13:17.1_0": "Tatthappamatto viharanto,",
  "bv13:17.2_0": "Nisajjaṭṭhānacaṅkame;",
  "bv13:17.3_0": "Abhiññāsu pāramiṁ gantvā,",
  "bv13:17.4_0": "Brahmalokamagañchahaṁ.",
  "bv13:18.1_0": "Sudassanaṁ nāma nagaraṁ,",
  "bv13:18.2_0": "sudatto nāma khattiyo;",
  "bv13:18.3_0": "Sudattā nāma janikā,",
  "bv13:18.4_0": "sumedhassa mahesino.",
  "bv13:19.1_0": "Navavassasahassāni,",
  "bv13:19.2_0": "Agāraṁ ajjha so vasi;",
  "bv13:19.3_0": "Sucandakañcanasirivaḍḍhā,",
  "bv13:19.4_0": "Tayo pāsādamuttamā.",
  "bv13:20.1_0": "Tisoḷasasahassāni,",
  "bv13:20.2_0": "nāriyo samalaṅkatā;",
  "bv13:20.3_0": "Sumanā nāma sā nārī,",
  "bv13:20.4_0": "punabbasu nāma atrajo.",
  "bv13:21.1_0": "Nimitte caturo disvā,",
  "bv13:21.2_0": "hatthiyānena nikkhami;",
  "bv13:21.3_0": "Anūnakaṁ aḍḍhamāsaṁ,",
  "bv13:21.4_0": "padhānaṁ padahī jino.",
  "bv13:22.1_0": "Brahmunā yācito santo,",
  "bv13:22.2_0": "sumedho lokanāyako;",
  "bv13:22.3_0": "Vatti cakkaṁ mahāvīro,",
  "bv13:22.4_0": "sudassanuyyānamuttame.",
  "bv13:23.1_0": "Saraṇo sabbakāmo ca,",
  "bv13:23.2_0": "ahesuṁ aggasāvakā;",
  "bv13:23.3_0": "Sāgaro nāmupaṭṭhāko,",
  "bv13:23.4_0": "sumedhassa mahesino.",
  "bv13:24.1_0": "Rāmā ceva surāmā ca,",
  "bv13:24.2_0": "ahesuṁ aggasāvikā;",
  "bv13:24.3_0": "Bodhi tassa bhagavato,",
  "bv13:24.4_0": "mahānīpoti vuccati.",
  "bv13:25.1_0": "Uruvelā yasavā ca,",
  "bv13:25.2_0": "ahesuṁ aggupaṭṭhakā;",
  "bv13:25.3_0": "Yasodharā sirimā ca,",
  "bv13:25.4_0": "ahesuṁ aggupaṭṭhikā.",
  "bv13:26.1_0": "Aṭṭhāsītiratanāni,",
  "bv13:26.2_0": "accuggato mahāmuni;",
  "bv13:26.3_0": "Obhāseti disā sabbā,",
  "bv13:26.4_0": "cando tāragaṇe yathā.",
  "bv13:27.1_0": "Cakkavattimaṇī nāma,",
  "bv13:27.2_0": "yathā tapati yojanaṁ;",
  "bv13:27.3_0": "Tatheva tassa ratanaṁ,",
  "bv13:27.4_0": "samantā pharati yojanaṁ.",
  "bv13:28.1_0": "Navutivassasahassāni,",
  "bv13:28.2_0": "āyu vijjati tāvade;",
  "bv13:28.3_0": "Tāvatā tiṭṭhamāno so,",
  "bv13:28.4_0": "tāresi janataṁ bahuṁ.",
  "bv13:29.1_0": "Tevijjachaḷabhiññehi,",
  "bv13:29.2_0": "balappattehi tādihi;",
  "bv13:29.3_0": "Samākulamidaṁ āsi,",
  "bv13:29.4_0": "arahantehi sādhuhi.",
  "bv13:30.1_0": "Tepi sabbe amitayasā,",
  "bv13:30.2_0": "vippamuttā nirūpadhī;",
  "bv13:30.3_0": "Ñāṇālokaṁ dassayitvā,",
  "bv13:30.4_0": "nibbutā te mahāyasā.",
  "bv13:31.1_0": "Sumedho jinavaro buddho,",
  "bv13:31.2_0": "Medhārāmamhi nibbuto;",
  "bv13:31.3_0": "Dhātuvitthārikaṁ āsi,",
  "bv13:31.4_0": "Tesu tesu padesato”ti.",
  "bv13:32.1_0": "Sumedhassa bhagavato vaṁso ekādasamo."
}