{
  "ja151:0.1_0": "Jātaka",
  "ja151:0.2_0": "Dukanipāta",
  "ja151:0.3_0": "Daḷhavagga",
  "ja151:0.4_0": "1. Rājovādajātaka",
  "ja151:1.1_0": "“Daḷhaṁ daḷhassa khipati,",
  "ja151:1.2_0": "balliko mudunā muduṁ;",
  "ja151:1.3_0": "Sādhumpi sādhunā jeti,",
  "ja151:1.4_0": "asādhumpi asādhunā;",
  "ja151:1.5_0": "Etādiso ayaṁ rājā,",
  "ja151:1.6_0": "maggā uyyāhi sārathi”.",
  "ja151:2.1_0": "“Akkodhena jine kodhaṁ,",
  "ja151:2.2_0": "asādhuṁ sādhunā jine;",
  "ja151:2.3_0": "Jine kadariyaṁ dānena,",
  "ja151:2.4_0": "saccenālikavādinaṁ;",
  "ja151:2.5_0": "Etādiso ayaṁ rājā,",
  "ja151:2.6_0": "maggā uyyāhi sārathī”ti.",
  "ja151:3.1_0": "Rājovādajātakaṁ paṭhamaṁ."
}