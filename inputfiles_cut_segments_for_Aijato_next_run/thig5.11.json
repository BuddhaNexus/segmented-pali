{
  "thig5.11:0.1_0": "Therīgāthā",
  "thig5.11:0.2_0": "Pañcakanipāta",
  "thig5.11:0.3_0": "11. Tiṁsamattātherīgāthā",
  "thig5.11:1.1_0": "“Musalāni gahetvāna,",
  "thig5.11:1.2_0": "dhaññaṁ koṭṭenti māṇavā;",
  "thig5.11:1.3_0": "Puttadārāni posentā,",
  "thig5.11:1.4_0": "dhanaṁ vindanti māṇavā.",
  "thig5.11:2.1_0": "Karotha buddhasāsanaṁ,",
  "thig5.11:2.2_0": "yaṁ katvā nānutappati;",
  "thig5.11:2.3_0": "Khippaṁ pādāni dhovitvā,",
  "thig5.11:2.4_0": "ekamante nisīdatha;",
  "thig5.11:2.5_0": "Cetosamathamanuyuttā,",
  "thig5.11:2.6_0": "karotha buddhasāsanaṁ”.",
  "thig5.11:3.1_0": "Tassā tā vacanaṁ sutvā,",
  "thig5.11:3.2_0": "paṭācārāya sāsanaṁ;",
  "thig5.11:3.3_0": "Pāde pakkhālayitvāna,",
  "thig5.11:3.4_0": "ekamantaṁ upāvisuṁ;",
  "thig5.11:3.5_0": "Cetosamathamanuyuttā,",
  "thig5.11:3.6_0": "akaṁsu buddhasāsanaṁ.",
  "thig5.11:4.1_0": "Rattiyā purime yāme,",
  "thig5.11:4.2_0": "pubbajātimanussaruṁ;",
  "thig5.11:4.3_0": "Rattiyā majjhime yāme,",
  "thig5.11:4.4_0": "dibbacakkhuṁ visodhayuṁ;",
  "thig5.11:4.5_0": "Rattiyā pacchime yāme,",
  "thig5.11:4.6_0": "tamokhandhaṁ padālayuṁ.",
  "thig5.11:5.1_0": "Uṭṭhāya pāde vandiṁsu,",
  "thig5.11:5.2_0": "“katā te anusāsanī;",
  "thig5.11:5.3_0": "Indaṁva devā tidasā,",
  "thig5.11:5.4_0": "saṅgāme aparājitaṁ;",
  "thig5.11:5.5_0": "Purakkhatvā vihassāma,",
  "thig5.11:5.6_0": "tevijjāmha anāsavā”ti.",
  "thig5.11:6.1_0": "Itthaṁ sudaṁ tiṁsamattā therī bhikkhuniyo paṭācārāya santike aññaṁ byākariṁsūti."
}