{
  "ja234:0.1_0": "Jātaka",
  "ja234:0.2_0": "Dukanipāta",
  "ja234:0.3_0": "Upāhanavagga",
  "ja234:0.4_0": "4. Asitābhūjātaka",
  "ja234:1.1_0": "“Tvameva dānimakara,",
  "ja234:1.2_0": "Yaṁ kāmo byagamā tayi;",
  "ja234:1.3_0": "Soyaṁ appaṭisandhiko,",
  "ja234:1.4_0": "Kharachinnaṁva renukaṁ”.",
  "ja234:2.1_0": "“Atricchaṁ atilobhena,",
  "ja234:2.2_0": "atilobhamadena ca;",
  "ja234:2.3_0": "Evaṁ hāyati atthamhā,",
  "ja234:2.4_0": "ahaṁva asitābhuyā”ti.",
  "ja234:3.1_0": "Asitābhūjātakaṁ catutthaṁ."
}