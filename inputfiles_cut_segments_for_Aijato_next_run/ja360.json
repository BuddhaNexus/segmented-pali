{
  "ja360:0.1_0": "Jātaka",
  "ja360:0.2_0": "Pañcakanipāta",
  "ja360:0.3_0": "Maṇikuṇḍalavagga",
  "ja360:0.4_0": "10. Suyonandījātaka",
  "ja360:1.1_0": "“Vāti gandho timirānaṁ,",
  "ja360:1.2_0": "kusamuddo ca ghosavā;",
  "ja360:1.3_0": "Dūre ito suyonandī,",
  "ja360:1.4_0": "tamba kāmā tudanti maṁ”.",
  "ja360:2.1_0": "“Kathaṁ samuddamatari,",
  "ja360:2.2_0": "Kathaṁ addakkhi sedumaṁ;",
  "ja360:2.3_0": "Kathaṁ tassā ca tuyhañca,",
  "ja360:2.4_0": "Ahu saggasamāgamo”.",
  "ja360:3.1_0": "“Kurukacchā payātānaṁ,",
  "ja360:3.2_0": "vāṇijānaṁ dhanesinaṁ;",
  "ja360:3.3_0": "Makarehi abhidā nāvā,",
  "ja360:3.4_0": "phalakenāhamaplaviṁ.",
  "ja360:4.1_0": "Sā maṁ saṇhena mudunā,",
  "ja360:4.2_0": "niccaṁ candanagandhinī;",
  "ja360:4.3_0": "Aṅgena uddharī bhaddā,",
  "ja360:4.4_0": "mātā puttaṁva orasaṁ.",
  "ja360:5.1_0": "Sā maṁ annena pānena,",
  "ja360:5.2_0": "vatthena sayanena ca;",
  "ja360:5.3_0": "Attanāpi ca mandakkhī,",
  "ja360:5.4_0": "evaṁ tamba vijānahī”ti.",
  "ja360:6.1_0": "Suyonandījātakaṁ dasamaṁ.",
  "ja360:6.2_0": "Maṇikuṇḍalavaggo paṭhamo.",
  "ja360:7.1_0": "Tassuddānaṁ",
  "ja360:8.1_0": "Atha jinavaro haritaṁ tiṇako,",
  "ja360:8.2_0": "Atha bhinnaplavo uragova ghaṭo;",
  "ja360:8.3_0": "Dariyā puna kuñjara bhūnahatā,",
  "ja360:8.4_0": "Migamuttamasaggavarena dasāti."
}