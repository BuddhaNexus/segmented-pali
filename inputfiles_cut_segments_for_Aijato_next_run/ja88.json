{
  "ja88:0.1_0": "Jātaka",
  "ja88:0.2_0": "Ekakanipāta",
  "ja88:0.3_0": "Apāyimhavagga",
  "ja88:0.4_0": "8. Sārambhajātaka",
  "ja88:1.1_0": "“Kalyāṇimeva muñceyya,",
  "ja88:1.2_0": "na hi muñceyya pāpikaṁ;",
  "ja88:1.3_0": "Mokkho kalyāṇiyā sādhu,",
  "ja88:1.4_0": "mutvā tappati pāpikan”ti.",
  "ja88:2.1_0": "Sārambhajātakaṁ aṭṭhamaṁ."
}