{
  "thig9.1:0.1_0": "Therīgāthā",
  "thig9.1:0.2_0": "Navakanipāta",
  "thig9.1:0.3_0": "1. Vaḍḍhamātutherīgāthā",
  "thig9.1:1.1_0": "“Mā su te vaḍḍha lokamhi,",
  "thig9.1:1.2_0": "vanatho āhu kudācanaṁ;",
  "thig9.1:1.3_0": "Mā puttaka punappunaṁ,",
  "thig9.1:1.4_0": "ahu dukkhassa bhāgimā.",
  "thig9.1:2.1_0": "Sukhañhi vaḍḍha munayo,",
  "thig9.1:2.2_0": "anejā chinnasaṁsayā;",
  "thig9.1:2.3_0": "Sītibhūtā damappattā,",
  "thig9.1:2.4_0": "viharanti anāsavā.",
  "thig9.1:3.1_0": "Tehānuciṇṇaṁ isībhi,",
  "thig9.1:3.2_0": "maggaṁ dassanapattiyā;",
  "thig9.1:3.3_0": "Dukkhassantakiriyāya,",
  "thig9.1:3.4_0": "tvaṁ vaḍḍha anubrūhaya”.",
  "thig9.1:4.1_0": "“Visāradāva bhaṇasi,",
  "thig9.1:4.2_0": "etamatthaṁ janetti me;",
  "thig9.1:4.3_0": "Maññāmi nūna māmike,",
  "thig9.1:4.4_0": "vanatho te na vijjati”.",
  "thig9.1:5.1_0": "“Ye keci vaḍḍha saṅkhārā,",
  "thig9.1:5.2_0": "hīnā ukkaṭṭhamajjhimā;",
  "thig9.1:5.3_0": "Aṇūpi aṇumattopi,",
  "thig9.1:5.4_0": "vanatho me na vijjati.",
  "thig9.1:6.1_0": "Sabbe me āsavā khīṇā,",
  "thig9.1:6.2_0": "appamattassa jhāyato;",
  "thig9.1:6.3_0": "Tisso vijjā anuppattā,",
  "thig9.1:6.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "thig9.1:7.1_0": "“Uḷāraṁ vata me mātā,",
  "thig9.1:7.2_0": "patodaṁ samavassari;",
  "thig9.1:7.3_0": "Paramatthasañhitā gāthā,",
  "thig9.1:7.4_0": "yathāpi anukampikā.",
  "thig9.1:8.1_0": "Tassāhaṁ vacanaṁ sutvā,",
  "thig9.1:8.2_0": "anusiṭṭhiṁ janettiyā;",
  "thig9.1:8.3_0": "Dhammasaṁvegamāpādiṁ,",
  "thig9.1:8.4_0": "yogakkhemassa pattiyā.",
  "thig9.1:9.1_0": "Sohaṁ padhānapahitatto,",
  "thig9.1:9.2_0": "rattindivamatandito;",
  "thig9.1:9.3_0": "Mātarā codito santo,",
  "thig9.1:9.4_0": "aphusiṁ santimuttamaṁ”.",
  "thig9.1:10.1_0": "…",
  "thig9.1:10.2_0": "Vaḍḍhamātā therī ….",
  "thig9.1:11.1_0": "Navakanipāto niṭṭhito."
}