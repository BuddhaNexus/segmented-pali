{
  "vv60:0.1_0": "Vimānavatthu",
  "vv60:0.2_0": "Purisavimāna",
  "vv60:0.3_0": "Mahārathavagga",
  "vv60:0.4_0": "10. Paṭhamanāgavimānavatthu",
  "vv60:1.1_0": "“Susukkakhandhaṁ abhiruyha nāgaṁ,",
  "vv60:1.2_0": "Akācinaṁ dantiṁ baliṁ mahājavaṁ;",
  "vv60:1.3_0": "Abhiruyha gajavaraṁ sukappitaṁ,",
  "vv60:1.4_0": "Idhāgamā vehāyasaṁ antalikkhe.",
  "vv60:2.1_0": "Nāgassa dantesu duvesu nimmitā,",
  "vv60:2.2_0": "Acchodakā paduminiyo suphullā;",
  "vv60:2.3_0": "Padumesu ca tūriyagaṇā pavajjare,",
  "vv60:2.4_0": "Imā ca naccanti manoharāyo.",
  "vv60:3.1_0": "Deviddhipattosi mahānubhāvo,",
  "vv60:3.2_0": "Manussabhūto kimakāsi puññaṁ;",
  "vv60:3.3_0": "Kenāsi evaṁ jalitānubhāvo,",
  "vv60:3.4_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv60:4.1_0": "So devaputto attamano,",
  "vv60:4.2_0": "moggallānena pucchito;",
  "vv60:4.3_0": "Pañhaṁ puṭṭho viyākāsi,",
  "vv60:4.4_0": "yassa kammassidaṁ phalaṁ.",
  "vv60:5.1_0": "“Aṭṭheva muttapupphāni,",
  "vv60:5.2_0": "kassapassa mahesino;",
  "vv60:5.3_0": "Thūpasmiṁ abhiropesiṁ,",
  "vv60:5.4_0": "pasanno sehi pāṇibhi.",
  "vv60:6.1_0": "Tena metādiso vaṇṇo,",
  "vv60:6.2_0": "…pe…",
  "vv60:6.3_0": "vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv60:7.1_0": "Paṭhamanāgavimānaṁ dasamaṁ."
}