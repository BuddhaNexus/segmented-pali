{
  "kv20.4:0.1_0": "Kathāvatthu",
  "kv20.4:0.2_0": "Mahāpaṇṇāsaka",
  "kv20.4:0.3_0": "Vīsatimavagga",
  "kv20.4:0.4_0": "Tiracchānakathā",
  "kv20.4:1.1_0": "Atthi devesu tiracchānagatāti?",
  "kv20.4:1.2_0": "Āmantā.",
  "kv20.4:1.3_0": "Atthi tiracchānagatesu devāti?",
  "kv20.4:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv20.4:1.5_0": "atthi devesu tiracchānagatāti?",
  "kv20.4:1.6_0": "Āmantā.",
  "kv20.4:1.7_0": "Devaloko tiracchānayonīti?",
  "kv20.4:1.8_0": "Na hevaṁ vattabbe …pe…",
  "kv20.4:1.9_0": "atthi devesu tiracchānagatāti?",
  "kv20.4:1.10_0": "Āmantā.",
  "kv20.4:1.11_0": "Atthi tattha kīṭā paṭaṅgā makasā makkhikā ahī vicchikā satapadī gaṇḍuppādāti?",
  "kv20.4:1.12_0": "Na hevaṁ vattabbe …pe….",
  "kv20.4:2.1_0": "Natthi devesu tiracchānagatāti?",
  "kv20.4:2.2_0": "Āmantā.",
  "kv20.4:2.3_0": "Nanu atthi tattha erāvaṇo nāma hatthināgo sahassayuttaṁ dibbaṁ yānanti?",
  "kv20.4:2.4_0": "Āmantā.",
  "kv20.4:2.5_0": "Hañci atthi tattha erāvaṇo nāma hatthināgo sahassayuttaṁ dibbaṁ yānaṁ, tena vata re vattabbe—",
  "kv20.4:2.6_0": "“atthi devesu tiracchānagatā”ti.",
  "kv20.4:3.1_0": "Atthi devesu tiracchānagatāti?",
  "kv20.4:3.2_0": "Āmantā.",
  "kv20.4:3.3_0": "Atthi tattha hatthibandhā assabandhā yāvasikā kāraṇikā bhattakārakāti?",
  "kv20.4:3.4_0": "Na hevaṁ vattabbe.",
  "kv20.4:3.5_0": "Tena hi natthi devesu tiracchānagatāti.",
  "kv20.4:3.6_0": "Tiracchānakathā niṭṭhitā."
}