{
  "ja494:0.1_0": "Jātaka",
  "ja494:0.2_0": "Pakiṇṇakanipāta",
  "ja494:0.3_0": "Sālikedāravagga",
  "ja494:0.4_0": "11. Sādhinajātaka",
  "ja494:1.1_0": "“Abbhuto vata lokasmiṁ,",
  "ja494:1.2_0": "uppajji lomahaṁsano;",
  "ja494:1.3_0": "Dibbo ratho pāturahu,",
  "ja494:1.4_0": "vedehassa yasassino”.",
  "ja494:2.1_0": "Devaputto mahiddhiko,",
  "ja494:2.2_0": "mātali devasārathi;",
  "ja494:2.3_0": "Nimantayittha rājānaṁ,",
  "ja494:2.4_0": "vedehaṁ mithilaggahaṁ.",
  "ja494:3.1_0": "“Ehimaṁ rathamāruyha,",
  "ja494:3.2_0": "rājaseṭṭha disampati;",
  "ja494:3.3_0": "Devā dassanakāmā te,",
  "ja494:3.4_0": "tāvatiṁsā saindakā;",
  "ja494:3.5_0": "Saramānā hi te devā,",
  "ja494:3.6_0": "sudhammāyaṁ samacchare”.",
  "ja494:4.1_0": "Tato ca rājā sādhino,",
  "ja494:4.2_0": "vedeho mithilaggaho;",
  "ja494:4.3_0": "Sahassayuttamāruyha,",
  "ja494:4.4_0": "agā devāna santike;",
  "ja494:4.5_0": "Taṁ devā paṭinandiṁsu,",
  "ja494:4.6_0": "disvā rājānamāgataṁ.",
  "ja494:5.1_0": "“Svāgataṁ te mahārāja,",
  "ja494:5.2_0": "atho te adurāgataṁ;",
  "ja494:5.3_0": "Nisīda dāni rājīsi,",
  "ja494:5.4_0": "devarājassa santike”.",
  "ja494:6.1_0": "Sakkopi paṭinandittha,",
  "ja494:6.2_0": "vedehaṁ mithilaggahaṁ;",
  "ja494:6.3_0": "Nimantayittha kāmehi,",
  "ja494:6.4_0": "āsanena ca vāsavo.",
  "ja494:7.1_0": "“Sādhu khosi anuppatto,",
  "ja494:7.2_0": "āvāsaṁ vasavattinaṁ;",
  "ja494:7.3_0": "Vasa devesu rājīsi,",
  "ja494:7.4_0": "sabbakāmasamiddhisu;",
  "ja494:7.5_0": "Tāvatiṁsesu devesu,",
  "ja494:7.6_0": "bhuñja kāme amānuse”.",
  "ja494:8.1_0": "“Ahaṁ pure saggagato ramāmi,",
  "ja494:8.2_0": "Naccehi gītehi ca vāditehi;",
  "ja494:8.3_0": "So dāni ajja na ramāmi sagge,",
  "ja494:8.4_0": "Āyuṁ nu khīṇo maraṇaṁ nu santike;",
  "ja494:8.5_0": "Udāhu mūḷhosmi janindaseṭṭha”.",
  "ja494:9.1_0": "“Na tāyu khīṇaṁ maraṇañca dūre,",
  "ja494:9.2_0": "Na cāpi mūḷho naravīraseṭṭha;",
  "ja494:9.3_0": "Tuyhañca puññāni parittakāni,",
  "ja494:9.4_0": "Yesaṁ vipākaṁ idha vedayittho.",
  "ja494:10.1_0": "Vasa devānubhāvena,",
  "ja494:10.2_0": "rājaseṭṭha disampati;",
  "ja494:10.3_0": "Tāvatiṁsesu devesu,",
  "ja494:10.4_0": "bhuñja kāme amānuse”.",
  "ja494:11.1_0": "“Yathā yācitakaṁ yānaṁ,",
  "ja494:11.2_0": "yathā yācitakaṁ dhanaṁ;",
  "ja494:11.3_0": "Evaṁsampadamevetaṁ,",
  "ja494:11.4_0": "yaṁ parato dānapaccayā.",
  "ja494:12.1_0": "Na cāhametamicchāmi,",
  "ja494:12.2_0": "yaṁ parato dānapaccayā;",
  "ja494:12.3_0": "Sayaṅkatāni puññāni,",
  "ja494:12.4_0": "taṁ me āveṇikaṁ dhanaṁ.",
  "ja494:13.1_0": "Sohaṁ gantvā manussesu,",
  "ja494:13.2_0": "kāhāmi kusalaṁ bahuṁ;",
  "ja494:13.3_0": "Dānena samacariyāya,",
  "ja494:13.4_0": "saṁyamena damena ca;",
  "ja494:13.5_0": "Yaṁ katvā sukhito hoti,",
  "ja494:13.6_0": "na ca pacchānutappati.",
  "ja494:14.1_0": "Imāni tāni khettāni,",
  "ja494:14.2_0": "imaṁ nikkhaṁ sukuṇḍalaṁ;",
  "ja494:14.3_0": "Imā tā haritānūpā,",
  "ja494:14.4_0": "imā najjo savantiyo.",
  "ja494:15.1_0": "Imā tā pokkharaṇī rammā,",
  "ja494:15.2_0": "cakkavākapakūjitā;",
  "ja494:15.3_0": "Mandālakehi sañchannā,",
  "ja494:15.4_0": "padumuppalakehi ca;",
  "ja494:15.5_0": "Yassimāni mamāyiṁsu,",
  "ja494:15.6_0": "kiṁ nu te disataṁ gatā.",
  "ja494:16.1_0": "Tānīdha khettāni so bhūmibhāgo,",
  "ja494:16.2_0": "Teyeva ārāmavanūpacārā;",
  "ja494:16.3_0": "Tameva mayhaṁ janataṁ apassato,",
  "ja494:16.4_0": "Suññaṁva me nārada khāyate disā”.",
  "ja494:17.1_0": "“Diṭṭhā mayā vimānāni,",
  "ja494:17.2_0": "obhāsentā catuddisā;",
  "ja494:17.3_0": "Sammukhā devarājassa,",
  "ja494:17.4_0": "tidasānañca sammukhā.",
  "ja494:18.1_0": "Vutthaṁ me bhavanaṁ dibyaṁ,",
  "ja494:18.2_0": "Bhuttā kāmā amānusā;",
  "ja494:18.3_0": "Tāvatiṁsesu devesu,",
  "ja494:18.4_0": "Sabbakāmasamiddhisu.",
  "ja494:19.1_0": "Sohaṁ etādisaṁ hitvā,",
  "ja494:19.2_0": "puññāyamhi idhāgato;",
  "ja494:19.3_0": "Dhammameva carissāmi,",
  "ja494:19.4_0": "nāhaṁ rajjena atthiko.",
  "ja494:20.1_0": "Adaṇḍāvacaraṁ maggaṁ,",
  "ja494:20.2_0": "sammāsambuddhadesitaṁ;",
  "ja494:20.3_0": "Taṁ maggaṁ paṭipajjissaṁ,",
  "ja494:20.4_0": "yena gacchanti subbatā”ti.",
  "ja494:21.1_0": "Sādhinajātakaṁ ekādasamaṁ."
}