{
  "tha-ap106:0.1_0": "Therāpadāna",
  "tha-ap106:0.2_0": "Bhikkhadāyivagga",
  "tha-ap106:0.3_0": "4. Padapūjakattheraapadāna",
  "tha-ap106:1.1_0": "“Siddhatthassa bhagavato,",
  "tha-ap106:1.2_0": "jātipupphamadāsahaṁ;",
  "tha-ap106:1.3_0": "Pādesu satta pupphāni,",
  "tha-ap106:1.4_0": "hāsenokiritāni me.",
  "tha-ap106:2.1_0": "Tena kammenahaṁ ajja,",
  "tha-ap106:2.2_0": "abhibhomi narāmare;",
  "tha-ap106:2.3_0": "Dhāremi antimaṁ dehaṁ,",
  "tha-ap106:2.4_0": "sammāsambuddhasāsane.",
  "tha-ap106:3.1_0": "Catunnavutito kappe,",
  "tha-ap106:3.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap106:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap106:3.4_0": "pupphapūjāyidaṁ phalaṁ.",
  "tha-ap106:4.1_0": "Samantagandhanāmāsuṁ,",
  "tha-ap106:4.2_0": "terasa cakkavattino;",
  "tha-ap106:4.3_0": "Ito pañcamake kappe,",
  "tha-ap106:4.4_0": "cāturantā janādhipā.",
  "tha-ap106:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap106:5.2_0": "…pe…",
  "tha-ap106:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap106:6.1_0": "Itthaṁ sudaṁ āyasmā padapūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap106:7.1_0": "Padapūjakattherassāpadānaṁ catutthaṁ."
}