{
  "tha-ap271:0.1_0": "Therāpadāna",
  "tha-ap271:0.2_0": "Padumukkhipavagga",
  "tha-ap271:0.3_0": "9 Upāgatāsayattheraapadāna",
  "tha-ap271:1.1_0": "“Himavantassa vemajjhe,",
  "tha-ap271:1.2_0": "saro āsi sunimmito;",
  "tha-ap271:1.3_0": "Tatthāhaṁ rakkhaso āsiṁ,",
  "tha-ap271:1.4_0": "heṭhasīlo bhayānako.",
  "tha-ap271:2.1_0": "Anukampako kāruṇiko,",
  "tha-ap271:2.2_0": "vipassī lokanāyako;",
  "tha-ap271:2.3_0": "Mamuddharitukāmo so,",
  "tha-ap271:2.4_0": "āgacchi mama santikaṁ.",
  "tha-ap271:3.1_0": "Upāgataṁ mahāvīraṁ,",
  "tha-ap271:3.2_0": "devadevaṁ narāsabhaṁ;",
  "tha-ap271:3.3_0": "Āsayā abhinikkhamma,",
  "tha-ap271:3.4_0": "avandiṁ satthuno ahaṁ.",
  "tha-ap271:4.1_0": "Ekanavutito kappe,",
  "tha-ap271:4.2_0": "yaṁ vandiṁ purisuttamaṁ;",
  "tha-ap271:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap271:4.4_0": "vandanāya idaṁ phalaṁ.",
  "tha-ap271:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap271:5.2_0": "…pe…",
  "tha-ap271:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap271:6.1_0": "Itthaṁ sudaṁ āyasmā upāgatāsayo thero imā gāthāyo abhāsitthāti.",
  "tha-ap271:7.1_0": "Upāgatāsayattherassāpadānaṁ navamaṁ."
}