{
  "thig1.8:0.1_0": "Therīgāthā",
  "thig1.8:0.2_0": "Ekakanipāta",
  "thig1.8:0.3_0": "8. Mittātherīgāthā",
  "thig1.8:1.1_0": "“Saddhāya pabbajitvāna,",
  "thig1.8:1.2_0": "mitte mittaratā bhava;",
  "thig1.8:1.3_0": "Bhāvehi kusale dhamme,",
  "thig1.8:1.4_0": "yogakkhemassa pattiyā”ti.",
  "thig1.8:2.1_0": "… Mittā therī …."
}