{
  "tha-ap304:0.1_0": "Therāpadāna",
  "tha-ap304:0.2_0": "Padumakesaravagga",
  "tha-ap304:0.3_0": "2. Sabbagandhiyattheraapadāna",
  "tha-ap304:1.1_0": "“Gandhamālaṁ mayā dinnaṁ,",
  "tha-ap304:1.2_0": "vipassissa mahesino;",
  "tha-ap304:1.3_0": "Adāsiṁ ujubhūtassa,",
  "tha-ap304:1.4_0": "koseyyavatthamuttamaṁ.",
  "tha-ap304:2.1_0": "Ekanavutito kappe,",
  "tha-ap304:2.2_0": "yaṁ vatthamadadiṁ pure;",
  "tha-ap304:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap304:2.4_0": "gandhadānassidaṁ phalaṁ.",
  "tha-ap304:3.1_0": "Ito pannarase kappe,",
  "tha-ap304:3.2_0": "suceḷo nāma khattiyo;",
  "tha-ap304:3.3_0": "Sattaratanasampanno,",
  "tha-ap304:3.4_0": "cakkavattī mahabbalo.",
  "tha-ap304:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap304:4.2_0": "…pe…",
  "tha-ap304:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap304:5.1_0": "Itthaṁ sudaṁ āyasmā sabbagandhiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap304:6.1_0": "Sabbagandhiyattherassāpadānaṁ dutiyaṁ."
}