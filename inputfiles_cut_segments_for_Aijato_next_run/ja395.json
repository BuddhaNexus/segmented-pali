{
  "ja395:0.1_0": "Jātaka",
  "ja395:0.2_0": "Chakkanipāta",
  "ja395:0.3_0": "Kharaputtavagga",
  "ja395:0.4_0": "10. Pārāvatajātaka",
  "ja395:1.1_0": "“Cirassaṁ vata passāmi,",
  "ja395:1.2_0": "sahāyaṁ maṇidhārinaṁ;",
  "ja395:1.3_0": "Sukatā massukuttiyā,",
  "ja395:1.4_0": "sobhate vata me sakhā”.",
  "ja395:2.1_0": "“Parūḷhakacchanakhalomo,",
  "ja395:2.2_0": "ahaṁ kammesu byāvaṭo;",
  "ja395:2.3_0": "Cirassaṁ nhāpitaṁ laddhā,",
  "ja395:2.4_0": "lomaṁ taṁ ajja hārayiṁ”.",
  "ja395:3.1_0": "“Yannu lomaṁ ahāresi,",
  "ja395:3.2_0": "dullabhaṁ laddha kappakaṁ;",
  "ja395:3.3_0": "Atha kiñcarahi te samma,",
  "ja395:3.4_0": "kaṇṭhe kiṇikiṇāyati”.",
  "ja395:4.1_0": "“Manussasukhumālānaṁ,",
  "ja395:4.2_0": "maṇi kaṇṭhesu lambati;",
  "ja395:4.3_0": "Tesāhaṁ anusikkhāmi,",
  "ja395:4.4_0": "mā tvaṁ maññi davā kataṁ.",
  "ja395:5.1_0": "Sacepimaṁ pihayasi,",
  "ja395:5.2_0": "massukuttiṁ sukāritaṁ;",
  "ja395:5.3_0": "Kārayissāmi te samma,",
  "ja395:5.4_0": "maṇiñcāpi dadāmi te”.",
  "ja395:6.1_0": "“Tvaññeva maṇinā channo,",
  "ja395:6.2_0": "Sukatāya ca massuyā;",
  "ja395:6.3_0": "Āmanta kho taṁ gacchāmi,",
  "ja395:6.4_0": "Piyaṁ me tavadassanan”ti.",
  "ja395:7.1_0": "Pārāvatajātakaṁ dasamaṁ.",
  "ja395:7.2_0": "Kharaputtavaggo dutiyo.",
  "ja395:8.1_0": "Tassuddānaṁ",
  "ja395:9.1_0": "Atha passa sasūci ca tuṇḍilako,",
  "ja395:9.2_0": "Miga mayhakapañcamapakkhivaro;",
  "ja395:9.3_0": "Atha pañjali vārija mejjha puna,",
  "ja395:9.4_0": "Atha vaṭṭa kapotavarena dasāti.",
  "ja395:10.1_0": "Atha vagguddānaṁ",
  "ja395:11.1_0": "Atha vaggaṁ pakittissaṁ,",
  "ja395:11.2_0": "chanipātaṁ varuttame;",
  "ja395:11.3_0": "Avāriyā ca kharo ca,",
  "ja395:11.4_0": "dve ca vuttā subyañjanāti.",
  "ja395:12.1_0": "Chakkanipātaṁ niṭṭhitaṁ."
}