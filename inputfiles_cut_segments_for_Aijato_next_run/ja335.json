{
  "ja335:0.1_0": "Jātaka",
  "ja335:0.2_0": "Catukkanipāta",
  "ja335:0.3_0": "Kokilavagga",
  "ja335:0.4_0": "5. Jambukajātaka",
  "ja335:1.1_0": "“Brahā pavaḍḍhakāyo so,",
  "ja335:1.2_0": "dīghadāṭho ca jambuka;",
  "ja335:1.3_0": "Na tvaṁ tattha kule jāto,",
  "ja335:1.4_0": "yattha gaṇhanti kuñjaraṁ”.",
  "ja335:2.1_0": "“Asīho sīhamānena,",
  "ja335:2.2_0": "yo attānaṁ vikubbati;",
  "ja335:2.3_0": "Kotthūva gajamāsajja,",
  "ja335:2.4_0": "seti bhūmyā anutthunaṁ.",
  "ja335:3.1_0": "Yasassino uttamapuggalassa,",
  "ja335:3.2_0": "Sañjātakhandhassa mahabbalassa;",
  "ja335:3.3_0": "Asamekkhiya thāmabalūpapattiṁ,",
  "ja335:3.4_0": "Sa seti nāgena hatoyaṁ jambuko.",
  "ja335:4.1_0": "Yo cīdha kammaṁ kurute pamāya,",
  "ja335:4.2_0": "Thāmabbalaṁ attani saṁviditvā;",
  "ja335:4.3_0": "Jappena mantena subhāsitena,",
  "ja335:4.4_0": "Parikkhavā so vipulaṁ jinātī”ti.",
  "ja335:5.1_0": "Jambukajātakaṁ pañcamaṁ."
}