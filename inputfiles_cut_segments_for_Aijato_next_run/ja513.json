{
  "ja513:0.1_0": "Jātaka",
  "ja513:0.2_0": "Tiṁsanipāta",
  "ja513:0.3_0": "Kiṁchandavagga",
  "ja513:0.4_0": "3. Jayaddisajātaka",
  "ja513:1.1_0": "“Cirassaṁ vata me udapādi ajja,",
  "ja513:1.2_0": "Bhakkho mahā sattamibhattakāle;",
  "ja513:1.3_0": "Kutosi ko vāsi tadiṅgha brūhi,",
  "ja513:1.4_0": "Ācikkha jātiṁ vidito yathāsi”.",
  "ja513:2.1_0": "“Pañcālarājā migavaṁ paviṭṭho,",
  "ja513:2.2_0": "Jayaddiso nāma yadissuto te;",
  "ja513:2.3_0": "Carāmi kacchāni vanāni cāhaṁ,",
  "ja513:2.4_0": "Pasadaṁ imaṁ khāda mamajja muñca”.",
  "ja513:3.1_0": "“Seneva tvaṁ paṇasi sassamāno,",
  "ja513:3.2_0": "Mamesa bhakkho pasado yaṁ vadesi;",
  "ja513:3.3_0": "Taṁ khādiyāna pasadaṁ jighaññaṁ,",
  "ja513:3.4_0": "Khādissaṁ pacchā na vilāpakālo”.",
  "ja513:4.1_0": "“Na catthi mokkho mama nikkayena,",
  "ja513:4.2_0": "Gantvāna paccāgamanāya paṇhe;",
  "ja513:4.3_0": "Taṁ saṅgaraṁ brāhmaṇassappadāya,",
  "ja513:4.4_0": "Saccānurakkhī punarāvajissaṁ”.",
  "ja513:5.1_0": "“Kiṁ kammajātaṁ anutappate tvaṁ,",
  "ja513:5.2_0": "Pattaṁ samīpaṁ maraṇassa rāja;",
  "ja513:5.3_0": "Ācikkha me taṁ api sakkuṇemu,",
  "ja513:5.4_0": "Anujānituṁ āgamanāya paṇhe”.",
  "ja513:6.1_0": "“Katā mayā brāhmaṇassa dhanāsā,",
  "ja513:6.2_0": "Taṁ saṅgaraṁ paṭimukkaṁ na muttaṁ;",
  "ja513:6.3_0": "Taṁ saṅgaraṁ brāhmaṇassappadāya,",
  "ja513:6.4_0": "Saccānurakkhī punarāvajissaṁ”.",
  "ja513:7.1_0": "“Yā te katā brāhmaṇassa dhanāsā,",
  "ja513:7.2_0": "Taṁ saṅgaraṁ paṭimukkaṁ na muttaṁ;",
  "ja513:7.3_0": "Taṁ saṅgaraṁ brāhmaṇassappadāya,",
  "ja513:7.4_0": "Saccānurakkhī punarāvajassu”.",
  "ja513:8.1_0": "Mutto ca so porisādassa hatthā,",
  "ja513:8.2_0": "Gantvā sakaṁ mandiraṁ kāmakāmī;",
  "ja513:8.3_0": "Taṁ saṅgaraṁ brāhmaṇassappadāya,",
  "ja513:8.4_0": "Āmantayī puttamalīnasattaṁ.",
  "ja513:9.1_0": "“Ajjeva rajjaṁ abhisiñcayassu,",
  "ja513:9.2_0": "Dhammaṁ cara sesu paresu cāpi;",
  "ja513:9.3_0": "Adhammakāro ca te māhu raṭṭhe,",
  "ja513:9.4_0": "Gacchāmahaṁ porisādassa ñatte”.",
  "ja513:10.1_0": "“Kiṁ kamma kubbaṁ tava deva pāva,",
  "ja513:10.2_0": "Nārādhayī taṁ tadicchāmi sotuṁ;",
  "ja513:10.3_0": "Yamajja rajjamhi udassaye tuvaṁ,",
  "ja513:10.4_0": "Rajjampi niccheyyaṁ tayā vināhaṁ”.",
  "ja513:11.1_0": "“Na kammunā vā vacasā va tāta,",
  "ja513:11.2_0": "Aparādhitohaṁ tuviyaṁ sarāmi;",
  "ja513:11.3_0": "Sandhiñca katvā purisādakena,",
  "ja513:11.4_0": "Saccānurakkhī punāhaṁ gamissaṁ”.",
  "ja513:12.1_0": "“Ahaṁ gamissāmi idheva hohi,",
  "ja513:12.2_0": "Natthi tato jīvato vippamokkho;",
  "ja513:12.3_0": "Sace tuvaṁ gacchasiyeva rāja,",
  "ja513:12.4_0": "Ahampi gacchāmi ubho na homa”.",
  "ja513:13.1_0": "“Addhā hi tāta satānesa dhammo,",
  "ja513:13.2_0": "Maraṇā ca me dukkhataraṁ tadassa;",
  "ja513:13.3_0": "Kammāsapādo taṁ yadā pacitvā,",
  "ja513:13.4_0": "Pasayha khāde bhidā rukkhasūle”.",
  "ja513:14.1_0": "“Pāṇena te pāṇamahaṁ nimissaṁ,",
  "ja513:14.2_0": "Mā tvaṁ agā porisādassa ñatte;",
  "ja513:14.3_0": "Etañca te pāṇamahaṁ nimissaṁ,",
  "ja513:14.4_0": "Tasmā mataṁ jīvitassa vaṇṇemi”.",
  "ja513:15.1_0": "Tato have dhitimā rājaputto,",
  "ja513:15.2_0": "Vanditvā mātu ca pitu ca pāde;",
  "ja513:15.3_0": "Dukhinissa mātā nipatā pathabyā,",
  "ja513:15.4_0": "Pitāssa paggayha bhujāni kandati.",
  "ja513:16.1_0": "Taṁ gacchantaṁ tāva pitā viditvā,",
  "ja513:16.2_0": "Parammukho vandati pañjalīko;",
  "ja513:16.3_0": "“Somo ca rājā varuṇo ca rājā,",
  "ja513:16.4_0": "Pajāpatī candimā sūriyo ca;",
  "ja513:16.5_0": "Etehi gutto purisādakamhā,",
  "ja513:16.6_0": "Anuññāto sotthi paccehi tāta”.",
  "ja513:17.1_0": "“Yaṁ daṇḍakirañño gatassa mātā,",
  "ja513:17.2_0": "Rāmassakāsi sotthānaṁ suguttā;",
  "ja513:17.3_0": "Taṁ te ahaṁ sotthānaṁ karomi,",
  "ja513:17.4_0": "Etena saccena sarantu devā;",
  "ja513:17.5_0": "Anuññāto sotthi paccehi putta”.",
  "ja513:18.1_0": "“Āvī raho vāpi manopadosaṁ,",
  "ja513:18.2_0": "Nāhaṁ sare jātu malīnasatte;",
  "ja513:18.3_0": "Etena saccena sarantu devā,",
  "ja513:18.4_0": "Anuññāto sotthi paccehi bhātika”.",
  "ja513:19.1_0": "“Yasmā ca me anadhimanosi sāmi,",
  "ja513:19.2_0": "Na cāpi me manasā appiyosi;",
  "ja513:19.3_0": "Etena saccena sarantu devā,",
  "ja513:19.4_0": "Anuññāto sotthi paccehi sāmi”.",
  "ja513:20.1_0": "“Brahā ujū cārumukho kutosi,",
  "ja513:20.2_0": "Na maṁ pajānāsi vane vasantaṁ;",
  "ja513:20.3_0": "Luddaṁ maṁ ñatvā ‘purisādako’ti,",
  "ja513:20.4_0": "Ko sotthimājānamidhāvajeyya”.",
  "ja513:21.1_0": "“Jānāmi ludda purisādako tvaṁ,",
  "ja513:21.2_0": "Na taṁ na jānāmi vane vasantaṁ;",
  "ja513:21.3_0": "Ahañca puttosmi jayaddisassa,",
  "ja513:21.4_0": "Mamajja khāda pituno pamokkhā”.",
  "ja513:22.1_0": "“Jānāmi puttoti jayaddisassa,",
  "ja513:22.2_0": "Tathā hi vo mukhavaṇṇo ubhinnaṁ;",
  "ja513:22.3_0": "Sudukkaraññeva kataṁ tavedaṁ,",
  "ja513:22.4_0": "Yo mattumicche pituno pamokkhā”.",
  "ja513:23.1_0": "“Na dukkaraṁ kiñci mahettha maññe,",
  "ja513:23.2_0": "Yo mattumicche pituno pamokkhā;",
  "ja513:23.3_0": "Mātu ca hetu paraloka gantvā,",
  "ja513:23.4_0": "Sukhena saggena ca sampayutto”.",
  "ja513:24.1_0": "“Ahañca kho attano pāpakiriyaṁ,",
  "ja513:24.2_0": "Āvī raho vāpi sare na jātu;",
  "ja513:24.3_0": "Saṅkhātajātīmaraṇohamasmi,",
  "ja513:24.4_0": "Yatheva me idha tathā parattha.",
  "ja513:25.1_0": "Khādajja maṁ dāni mahānubhāva,",
  "ja513:25.2_0": "Karassu kiccāni imaṁ sarīraṁ;",
  "ja513:25.3_0": "Rukkhassa vā te papatāmi aggā,",
  "ja513:25.4_0": "Chādayamāno mayhaṁ tvamadesi maṁsaṁ”.",
  "ja513:26.1_0": "“Idañca te ruccati rājaputta,",
  "ja513:26.2_0": "Cajesi pāṇaṁ pituno pamokkhā;",
  "ja513:26.3_0": "Tasmā hi so tvaṁ taramānarūpo,",
  "ja513:26.4_0": "Sambhañja kaṭṭhāni jalehi aggiṁ”.",
  "ja513:27.1_0": "Tato have dhitimā rājaputto,",
  "ja513:27.2_0": "Dāruṁ samāhatvā mahantamaggiṁ;",
  "ja513:27.3_0": "Sandīpayitvā paṭivedayittha,",
  "ja513:27.4_0": "“Ādīpito dāni mahāyamaggi”.",
  "ja513:28.1_0": "“Khādajja maṁ dāni pasayhakāri,",
  "ja513:28.2_0": "Kiṁ maṁ muhuṁ pekkhasi haṭṭhalomo;",
  "ja513:28.3_0": "Tathā tathā tuyhamahaṁ karomi,",
  "ja513:28.4_0": "Yathā yathā maṁ chādayamāno adesi”.",
  "ja513:29.1_0": "“Ko tādisaṁ arahati khāditāye,",
  "ja513:29.2_0": "Dhamme ṭhitaṁ saccavādiṁ vadaññuṁ;",
  "ja513:29.3_0": "Muddhāpi tassa viphaleyya sattadhā,",
  "ja513:29.4_0": "Yo tādisaṁ saccavādiṁ adeyya”.",
  "ja513:30.1_0": "“Idañhi so brāhmaṇaṁ maññamāno,",
  "ja513:30.2_0": "Saso avāsesi sake sarīre;",
  "ja513:30.3_0": "Teneva so candimā devaputto,",
  "ja513:30.4_0": "Sasatthuto kāmaduhajja yakkha”.",
  "ja513:31.1_0": "“Cando yathā rāhumukhā pamutto,",
  "ja513:31.2_0": "Virocate pannaraseva bhāṇumā;",
  "ja513:31.3_0": "Evaṁ tuvaṁ porisādā pamutto,",
  "ja513:31.4_0": "Viroca kapile mahānubhāva;",
  "ja513:31.5_0": "Āmodayaṁ pitaraṁ mātarañca,",
  "ja513:31.6_0": "Sabbo ca te nandatu ñātipakkho”.",
  "ja513:32.1_0": "Tato have dhitimā rājaputto,",
  "ja513:32.2_0": "Katañjalī pariyāya porisādaṁ;",
  "ja513:32.3_0": "Anuññāto sotthi sukhī arogo,",
  "ja513:32.4_0": "Paccāgamā kapilamalīnasatto.",
  "ja513:33.1_0": "Taṁ negamā jānapadā ca sabbe,",
  "ja513:33.2_0": "Hatthārohā rathikā pattikā ca;",
  "ja513:33.3_0": "Namassamānā pañjalikā upāgamuṁ,",
  "ja513:33.4_0": "“Namatthu te dukkarakārakosī”ti.",
  "ja513:34.1_0": "Jayaddisajātakaṁ tatiyaṁ."
}