{
  "ja443:0.1_0": "Jātaka",
  "ja443:0.2_0": "Dasakanipāta",
  "ja443:0.3_0": "Catudvāravagga",
  "ja443:0.4_0": "5. Cūḷabodhijātaka",
  "ja443:1.1_0": "“Yo te imaṁ visālakkhiṁ,",
  "ja443:1.2_0": "piyaṁ sammhitabhāsiniṁ;",
  "ja443:1.3_0": "Ādāya balā gaccheyya,",
  "ja443:1.4_0": "kiṁ nu kayirāsi brāhmaṇa”.",
  "ja443:2.1_0": "“Uppajje me na mucceyya,",
  "ja443:2.2_0": "na me mucceyya jīvato;",
  "ja443:2.3_0": "Rajaṁva vipulā vuṭṭhi,",
  "ja443:2.4_0": "khippameva nivāraye”.",
  "ja443:3.1_0": "“Yannu pubbe vikatthittho,",
  "ja443:3.2_0": "balamhiva apassito;",
  "ja443:3.3_0": "Svajja tuṇhikato dāni,",
  "ja443:3.4_0": "saṅghāṭiṁ sibbamacchasi”.",
  "ja443:4.1_0": "“Uppajji me na muccittha,",
  "ja443:4.2_0": "na me muccittha jīvato;",
  "ja443:4.3_0": "Rajaṁva vipulā vuṭṭhi,",
  "ja443:4.4_0": "khippameva nivārayiṁ”.",
  "ja443:5.1_0": "“Kiṁ te uppajji no mucci,",
  "ja443:5.2_0": "kiṁ te na mucci jīvato;",
  "ja443:5.3_0": "Rajaṁva vipulā vuṭṭhi,",
  "ja443:5.4_0": "katamaṁ tvaṁ nivārayi”.",
  "ja443:6.1_0": "“Yamhi jāte na passati,",
  "ja443:6.2_0": "ajāte sādhu passati;",
  "ja443:6.3_0": "So me uppajji no mucci,",
  "ja443:6.4_0": "kodho dummedhagocaro.",
  "ja443:7.1_0": "Yena jātena nandanti,",
  "ja443:7.2_0": "amittā dukkhamesino;",
  "ja443:7.3_0": "So me uppajji no mucci,",
  "ja443:7.4_0": "kodho dummedhagocaro.",
  "ja443:8.1_0": "Yasmiñca jāyamānamhi,",
  "ja443:8.2_0": "sadatthaṁ nāvabujjhati;",
  "ja443:8.3_0": "So me uppajji no mucci,",
  "ja443:8.4_0": "kodho dummedhagocaro.",
  "ja443:9.1_0": "Yenābhibhūto kusalaṁ jahāti,",
  "ja443:9.2_0": "Parakkare vipulañcāpi atthaṁ;",
  "ja443:9.3_0": "Sa bhīmaseno balavā pamaddī,",
  "ja443:9.4_0": "Kodho mahārāja na me amuccatha.",
  "ja443:10.1_0": "Kaṭṭhasmiṁ matthamānasmiṁ,",
  "ja443:10.2_0": "Pāvako nāma jāyati;",
  "ja443:10.3_0": "Tameva kaṭṭhaṁ ḍahati,",
  "ja443:10.4_0": "Yasmā so jāyate gini.",
  "ja443:11.1_0": "Evaṁ mandassa posassa,",
  "ja443:11.2_0": "bālassa avijānato;",
  "ja443:11.3_0": "Sārambhā jāyate kodho,",
  "ja443:11.4_0": "sopi teneva ḍayhati.",
  "ja443:12.1_0": "Aggīva tiṇakaṭṭhasmiṁ,",
  "ja443:12.2_0": "kodho yassa pavaḍḍhati;",
  "ja443:12.3_0": "Nihīyati tassa yaso,",
  "ja443:12.4_0": "kāḷapakkheva candimā.",
  "ja443:13.1_0": "Anedho dhūmaketūva,",
  "ja443:13.2_0": "kodho yassūpasammati;",
  "ja443:13.3_0": "Āpūrati tassa yaso,",
  "ja443:13.4_0": "sukkapakkheva candimā”ti.",
  "ja443:14.1_0": "Cūḷabodhijātakaṁ pañcamaṁ."
}