{
  "ja409:0.1_0": "Jātaka",
  "ja409:0.2_0": "Sattakanipāta",
  "ja409:0.3_0": "Gandhāravagga",
  "ja409:0.4_0": "4. Daḷhadhammajātaka",
  "ja409:1.1_0": "“Ahañce daḷhadhammassa,",
  "ja409:1.2_0": "vahantī nābhirādhayiṁ;",
  "ja409:1.3_0": "Dharantī urasi sallaṁ,",
  "ja409:1.4_0": "yuddhe vikkantacārinī.",
  "ja409:2.1_0": "Nūna rājā na jānāti,",
  "ja409:2.2_0": "mama vikkamaporisaṁ;",
  "ja409:2.3_0": "Saṅgāme sukatantāni,",
  "ja409:2.4_0": "dūtavippahitāni ca.",
  "ja409:3.1_0": "Sā nūnāhaṁ marissāmi,",
  "ja409:3.2_0": "abandhu aparāyinī;",
  "ja409:3.3_0": "Tadā hi kumbhakārassa,",
  "ja409:3.4_0": "dinnā chakaṇahārikā”.",
  "ja409:4.1_0": "“Yāvatāsīsatī poso,",
  "ja409:4.2_0": "tāvadeva pavīṇati;",
  "ja409:4.3_0": "Atthāpāye jahanti naṁ,",
  "ja409:4.4_0": "oṭṭhibyādhiṁva khattiyo.",
  "ja409:5.1_0": "Yo pubbe katakalyāṇo,",
  "ja409:5.2_0": "katattho nāvabujjhati;",
  "ja409:5.3_0": "Atthā tassa palujjanti,",
  "ja409:5.4_0": "ye honti abhipatthitā.",
  "ja409:6.1_0": "Yo pubbe katakalyāṇo,",
  "ja409:6.2_0": "katattho manubujjhati;",
  "ja409:6.3_0": "Atthā tassa pavaḍḍhanti,",
  "ja409:6.4_0": "ye honti abhipatthitā.",
  "ja409:7.1_0": "Taṁ vo vadāmi bhaddante,",
  "ja409:7.2_0": "yāvantettha samāgatā;",
  "ja409:7.3_0": "Sabbe kataññuno hotha,",
  "ja409:7.4_0": "ciraṁ saggamhi ṭhassathā”ti.",
  "ja409:8.1_0": "Daḷhadhammajātakaṁ catutthaṁ."
}