{
  "thag1.10:0.1_0": "Theragāthā",
  "thag1.10:0.2_0": "Ekakanipāta",
  "thag1.10:0.3_0": "Paṭhamavagga",
  "thag1.10:0.4_0": "1.10. Puṇṇamāsattheragāthā",
  "thag1.10:1.1_0": "“Vihari apekkhaṁ idha vā huraṁ vā,",
  "thag1.10:1.2_0": "Yo vedagū samito yatatto;",
  "thag1.10:1.3_0": "Sabbesu dhammesu anūpalitto,",
  "thag1.10:1.4_0": "Lokassa jaññā udayabbayañcā”ti.",
  "thag1.10:1.5_0": "Itthaṁ sudaṁ āyasmā puṇṇamāso thero gāthaṁ abhāsitthāti.",
  "thag1.10:1.6_0": "vaggo paṭhamo.",
  "thag1.10:2.0_0": "Tassuddānaṁ",
  "thag1.10:2.1_0": "Subhūti koṭṭhiko thero,",
  "thag1.10:2.2_0": "kaṅkhārevatasammato;",
  "thag1.10:2.3_0": "Mantāṇiputto dabbo ca,",
  "thag1.10:2.4_0": "sītavaniyo ca bhalliyo;",
  "thag1.10:2.5_0": "Vīro pilindavaccho ca,",
  "thag1.10:2.6_0": "puṇṇamāso tamonudoti."
}