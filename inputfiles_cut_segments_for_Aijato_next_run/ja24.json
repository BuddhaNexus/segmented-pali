{
  "ja24:0.1_0": "Jātaka",
  "ja24:0.2_0": "Ekakanipāta",
  "ja24:0.3_0": "Kuruṅgavagga",
  "ja24:0.4_0": "4. Ājaññajātaka",
  "ja24:1.1_0": "“Yadā yadā yattha yadā,",
  "ja24:1.2_0": "yattha yattha yadā yadā;",
  "ja24:1.3_0": "Ājañño kurute vegaṁ,",
  "ja24:1.4_0": "hāyanti tattha vāḷavā”ti.",
  "ja24:2.1_0": "Ājaññajātakaṁ catutthaṁ."
}