{
  "vv3:0.1_0": "Vimānavatthu",
  "vv3:0.2_0": "Itthivimāna",
  "vv3:0.3_0": "Pīṭhavagga",
  "vv3:0.4_0": "3. Tatiyapīṭhavimānavatthu",
  "vv3:1.1_0": "“Pīṭhaṁ te sovaṇṇamayaṁ uḷāraṁ,",
  "vv3:1.2_0": "Manojavaṁ gacchati yenakāmaṁ;",
  "vv3:1.3_0": "Alaṅkate malyadhare suvatthe,",
  "vv3:1.4_0": "Obhāsasi vijjurivabbhakūṭaṁ.",
  "vv3:2.1_0": "Kena tetādiso vaṇṇo,",
  "vv3:2.2_0": "kena te idha mijjhati;",
  "vv3:2.3_0": "Uppajjanti ca te bhogā,",
  "vv3:2.4_0": "ye keci manaso piyā.",
  "vv3:3.1_0": "Pucchāmi taṁ devi mahānubhāve,",
  "vv3:3.2_0": "Manussabhūtā kimakāsi puññaṁ;",
  "vv3:3.3_0": "Kenāsi evaṁ jalitānubhāvā,",
  "vv3:3.4_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv3:4.1_0": "Sā devatā attamanā,",
  "vv3:4.2_0": "moggallānena pucchitā;",
  "vv3:4.3_0": "Pañhaṁ puṭṭhā viyākāsi,",
  "vv3:4.4_0": "yassa kammassidaṁ phalaṁ.",
  "vv3:5.1_0": "“Appassa kammassa phalaṁ mamedaṁ,",
  "vv3:5.2_0": "Yenamhi evaṁ jalitānubhāvā;",
  "vv3:5.3_0": "Ahaṁ manussesu manussabhūtā,",
  "vv3:5.4_0": "Purimāya jātiyā manussaloke.",
  "vv3:6.1_0": "Addasaṁ virajaṁ bhikkhuṁ,",
  "vv3:6.2_0": "vippasannamanāvilaṁ;",
  "vv3:6.3_0": "Tassa adāsahaṁ pīṭhaṁ,",
  "vv3:6.4_0": "pasannā sehi pāṇibhi.",
  "vv3:7.1_0": "Tena metādiso vaṇṇo,",
  "vv3:7.2_0": "tena me idha mijjhati;",
  "vv3:7.3_0": "Uppajjanti ca me bhogā,",
  "vv3:7.4_0": "ye keci manaso piyā.",
  "vv3:8.1_0": "Akkhāmi te bhikkhu mahānubhāva,",
  "vv3:8.2_0": "Manussabhūtā yamakāsi puññaṁ;",
  "vv3:8.3_0": "Tenamhi evaṁ jalitānubhāvā,",
  "vv3:8.4_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv3:9.1_0": "Tatiyapīṭhavimānaṁ tatiyaṁ."
}