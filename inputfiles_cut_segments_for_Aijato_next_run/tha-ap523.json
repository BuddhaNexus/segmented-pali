{
  "tha-ap523:0.1_0": "Therāpadāna",
  "tha-ap523:0.2_0": "Tiṇadāyakavagga",
  "tha-ap523:0.3_0": "1. Tiṇamuṭṭhidāyakattheraapadāna",
  "tha-ap523:1.1_0": "“Himavantassāvidūre,",
  "tha-ap523:1.2_0": "lambako nāma pabbato;",
  "tha-ap523:1.3_0": "Tattheva tisso sambuddho,",
  "tha-ap523:1.4_0": "abbhokāsamhi caṅkami.",
  "tha-ap523:2.1_0": "Migaluddo pure āsiṁ,",
  "tha-ap523:2.2_0": "araññe kānane ahaṁ;",
  "tha-ap523:2.3_0": "Disvāna taṁ devadevaṁ,",
  "tha-ap523:2.4_0": "tiṇamuṭṭhimadāsahaṁ.",
  "tha-ap523:3.1_0": "Nisīdanatthaṁ buddhassa,",
  "tha-ap523:3.2_0": "datvā cittaṁ pasādayiṁ;",
  "tha-ap523:3.3_0": "Sambuddhaṁ abhivādetvā,",
  "tha-ap523:3.4_0": "pakkāmiṁ uttarāmukho.",
  "tha-ap523:4.1_0": "Aciraṁ gatamattassa,",
  "tha-ap523:4.2_0": "migarājā apothayi;",
  "tha-ap523:4.3_0": "Sīhena pothito santo,",
  "tha-ap523:4.4_0": "tattha kālaṅkato ahaṁ.",
  "tha-ap523:5.1_0": "Āsanne me kataṁ kammaṁ,",
  "tha-ap523:5.2_0": "buddhaseṭṭhe anāsave;",
  "tha-ap523:5.3_0": "Sumutto saravegova,",
  "tha-ap523:5.4_0": "devalokamagañchahaṁ.",
  "tha-ap523:6.1_0": "Yūpo tattha subho āsi,",
  "tha-ap523:6.2_0": "puññakammābhinimmito;",
  "tha-ap523:6.3_0": "Sahassakaṇḍo satabheṇḍu,",
  "tha-ap523:6.4_0": "dhajālu haritāmayo.",
  "tha-ap523:7.1_0": "Pabhā niddhāvate tassa,",
  "tha-ap523:7.2_0": "sataraṁsīva uggato;",
  "tha-ap523:7.3_0": "Ākiṇṇo devakaññāhi,",
  "tha-ap523:7.4_0": "āmodiṁ kāmakāmihaṁ.",
  "tha-ap523:8.1_0": "Devalokā cavitvāna,",
  "tha-ap523:8.2_0": "sukkamūlena codito;",
  "tha-ap523:8.3_0": "Āgantvāna manussattaṁ,",
  "tha-ap523:8.4_0": "pattomhi āsavakkhayaṁ.",
  "tha-ap523:9.1_0": "Catunnavutito kappe,",
  "tha-ap523:9.2_0": "nisīdanamadāsahaṁ;",
  "tha-ap523:9.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap523:9.4_0": "tiṇamuṭṭhe idaṁ phalaṁ.",
  "tha-ap523:10.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap523:10.2_0": "bhavā sabbe samūhatā;",
  "tha-ap523:10.3_0": "Nāgova bandhanaṁ chetvā,",
  "tha-ap523:10.4_0": "viharāmi anāsavo.",
  "tha-ap523:11.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap523:11.2_0": "mama buddhassa santike;",
  "tha-ap523:11.3_0": "Tisso vijjā anuppattā,",
  "tha-ap523:11.4_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap523:12.1_0": "Paṭisambhidā catasso,",
  "tha-ap523:12.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap523:12.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap523:12.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap523:13.1_0": "Itthaṁ sudaṁ āyasmā tiṇamuṭṭhidāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap523:14.1_0": "Tiṇamuṭṭhidāyakattherassāpadānaṁ paṭhamaṁ."
}