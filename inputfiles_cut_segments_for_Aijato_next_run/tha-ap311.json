{
  "tha-ap311:0.1_0": "Therāpadāna",
  "tha-ap311:0.2_0": "Padumakesaravagga",
  "tha-ap311:0.3_0": "9 Buddhasaññakattheraapadāna",
  "tha-ap311:1.1_0": "“Udentaṁ sataraṁsiṁva,",
  "tha-ap311:1.2_0": "pītaraṁsiṁva bhāṇumaṁ;",
  "tha-ap311:1.3_0": "Vanantaragataṁ santaṁ,",
  "tha-ap311:1.4_0": "lokajeṭṭhaṁ narāsabhaṁ.",
  "tha-ap311:2.1_0": "Addasaṁ supinantena,",
  "tha-ap311:2.2_0": "siddhatthaṁ lokanāyakaṁ;",
  "tha-ap311:2.3_0": "Tattha cittaṁ pasādetvā,",
  "tha-ap311:2.4_0": "sugatiṁ upapajjahaṁ.",
  "tha-ap311:3.1_0": "Catunnavutito kappe,",
  "tha-ap311:3.2_0": "yaṁ saññamalabhiṁ tadā;",
  "tha-ap311:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap311:3.4_0": "buddhasaññāyidaṁ phalaṁ.",
  "tha-ap311:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap311:4.2_0": "…pe…",
  "tha-ap311:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap311:5.1_0": "Itthaṁ sudaṁ āyasmā buddhasaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap311:6.1_0": "Buddhasaññakattherassāpadānaṁ navamaṁ."
}