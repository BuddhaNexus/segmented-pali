{
  "ja155:0.1_0": "Jātaka",
  "ja155:0.2_0": "Dukanipāta",
  "ja155:0.3_0": "Daḷhavagga",
  "ja155:0.4_0": "5. Bhaggajātaka",
  "ja155:1.1_0": "“Jīva vassasataṁ bhagga,",
  "ja155:1.2_0": "aparāni ca vīsatiṁ;",
  "ja155:1.3_0": "Mā maṁ pisācā khādantu,",
  "ja155:1.4_0": "jīva tvaṁ sarado sataṁ”.",
  "ja155:2.1_0": "“Tvampi vassasataṁ jīva,",
  "ja155:2.2_0": "aparāni ca vīsatiṁ;",
  "ja155:2.3_0": "Visaṁ pisācā khādantu,",
  "ja155:2.4_0": "jīva tvaṁ sarado satan”ti.",
  "ja155:3.1_0": "Bhaggajātakaṁ pañcamaṁ."
}