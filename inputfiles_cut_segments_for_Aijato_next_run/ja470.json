{
  "ja470:0.1_0": "Jātaka",
  "ja470:0.2_0": "Dvādasakanipāta",
  "ja470:0.3_0": "Cūḷakuṇālavagga",
  "ja470:0.4_0": "7. Kosiyajātaka",
  "ja470:1.1_0": "“Neva kiṇāmi napi vikkiṇāmi,",
  "ja470:1.2_0": "Na cāpi me sannicayo ca atthi;",
  "ja470:1.3_0": "Sukiccharūpaṁ vatidaṁ parittaṁ,",
  "ja470:1.4_0": "Patthodano nālamayaṁ duvinnaṁ”.",
  "ja470:2.1_0": "“Appamhā appakaṁ dajjā,",
  "ja470:2.2_0": "anumajjhato majjhakaṁ;",
  "ja470:2.3_0": "Bahumhā bahukaṁ dajjā,",
  "ja470:2.4_0": "adānaṁ nupapajjati.",
  "ja470:3.1_0": "Taṁ taṁ vadāmi kosiya,",
  "ja470:3.2_0": "dehi dānāni bhuñja ca”.",
  "ja470:3.3_0": "Ariyamaggaṁ samārūha,",
  "ja470:3.4_0": "nekāsī labhate sukhaṁ.",
  "ja470:4.1_0": "“Moghañcassa hutaṁ hoti,",
  "ja470:4.2_0": "moghañcāpi samīhitaṁ;",
  "ja470:4.3_0": "Atithismiṁ yo nisinnasmiṁ,",
  "ja470:4.4_0": "eko bhuñjati bhojanaṁ.",
  "ja470:5.1_0": "Taṁ taṁ vadāmi kosiya,",
  "ja470:5.2_0": "dehi dānāni bhuñja ca;",
  "ja470:5.3_0": "Ariyamaggaṁ samārūha,",
  "ja470:5.4_0": "nekāsī labhate sukhaṁ”.",
  "ja470:6.1_0": "“Saccañcassa hutaṁ hoti,",
  "ja470:6.2_0": "saccañcāpi samīhitaṁ;",
  "ja470:6.3_0": "Atithismiṁ yo nisinnasmiṁ,",
  "ja470:6.4_0": "neko bhuñjati bhojanaṁ.",
  "ja470:7.1_0": "Taṁ taṁ vadāmi kosiya,",
  "ja470:7.2_0": "dehi dānāni bhuñja ca;",
  "ja470:7.3_0": "Ariyamaggaṁ samārūha,",
  "ja470:7.4_0": "nekāsī labhate sukhaṁ”.",
  "ja470:8.1_0": "“Sarañca juhati poso,",
  "ja470:8.2_0": "bahukāya gayāya ca;",
  "ja470:8.3_0": "Doṇe timbarutitthasmiṁ,",
  "ja470:8.4_0": "sīghasote mahāvahe.",
  "ja470:9.1_0": "Atra cassa hutaṁ hoti,",
  "ja470:9.2_0": "atra cassa samīhitaṁ;",
  "ja470:9.3_0": "Atithismiṁ yo nisinnasmiṁ,",
  "ja470:9.4_0": "neko bhuñjati bhojanaṁ.",
  "ja470:10.1_0": "Taṁ taṁ vadāmi kosiya,",
  "ja470:10.2_0": "dehi dānāni bhuñja ca;",
  "ja470:10.3_0": "Ariyamaggaṁ samārūha,",
  "ja470:10.4_0": "nekāsī labhate sukhaṁ”.",
  "ja470:11.1_0": "“Baḷisañhi so nigilati,",
  "ja470:11.2_0": "dīghasuttaṁ sabandhanaṁ;",
  "ja470:11.3_0": "Atithismiṁ yo nisinnasmiṁ,",
  "ja470:11.4_0": "eko bhuñjati bhojanaṁ.",
  "ja470:12.1_0": "Taṁ taṁ vadāmi kosiya,",
  "ja470:12.2_0": "dehi dānāni bhuñja ca;",
  "ja470:12.3_0": "Ariyamaggaṁ samārūha,",
  "ja470:12.4_0": "nekāsī labhate sukhaṁ”.",
  "ja470:13.1_0": "“Uḷāravaṇṇā vata brāhmaṇā ime,",
  "ja470:13.2_0": "Ayañca vo sunakho kissa hetu;",
  "ja470:13.3_0": "Uccāvacaṁ vaṇṇanibhaṁ vikubbati,",
  "ja470:13.4_0": "Akkhātha no brāhmaṇā ke nu tumhe”.",
  "ja470:14.1_0": "“Cando ca suriyo ca ubho idhāgatā,",
  "ja470:14.2_0": "Ayaṁ pana mātali devasārathi;",
  "ja470:14.3_0": "Sakkohamasmi tidasānamindo,",
  "ja470:14.4_0": "Eso ca kho pañcasikhoti vuccati”.",
  "ja470:15.1_0": "“Pāṇissarā mudiṅgā ca,",
  "ja470:15.2_0": "murajālambarāni ca;",
  "ja470:15.3_0": "Suttamenaṁ pabodhenti,",
  "ja470:15.4_0": "paṭibuddho ca nandati”.",
  "ja470:16.1_0": "“Ye kecime maccharino kadariyā,",
  "ja470:16.2_0": "Paribhāsakā samaṇabrāhmaṇānaṁ;",
  "ja470:16.3_0": "Idheva nikkhippa sarīradehaṁ,",
  "ja470:16.4_0": "Kāyassa bhedā nirayaṁ vajanti”.",
  "ja470:17.1_0": "“Ye kecime suggatimāsamānā,",
  "ja470:17.2_0": "Dhamme ṭhitā saṁyame saṁvibhāge;",
  "ja470:17.3_0": "Idheva nikkhippa sarīradehaṁ,",
  "ja470:17.4_0": "Kāyassa bhedā sugatiṁ vajanti.",
  "ja470:18.1_0": "Tvaṁ nosi ñāti purimāsu jātisu,",
  "ja470:18.2_0": "So maccharī rosako pāpadhammo;",
  "ja470:18.3_0": "Taveva atthāya idhāgatamhā,",
  "ja470:18.4_0": "Mā pāpadhammo nirayaṁ gamittha”.",
  "ja470:19.1_0": "“Addhā hi maṁ vo hitakāmā,",
  "ja470:19.2_0": "yaṁ maṁ samanusāsatha;",
  "ja470:19.3_0": "Sohaṁ tathā karissāmi,",
  "ja470:19.4_0": "sabbaṁ vuttaṁ hitesibhi.",
  "ja470:20.1_0": "Esāhamajjeva upāramāmi,",
  "ja470:20.2_0": "Na cāpihaṁ kiñci kareyya pāpaṁ;",
  "ja470:20.3_0": "Na cāpi me kiñci adeyyamatthi,",
  "ja470:20.4_0": "Na cāpidatvā udakaṁ pivāmi.",
  "ja470:21.1_0": "Evañca me dadato sabbakālaṁ,",
  "ja470:21.2_0": "Bhogā ime vāsava khīyissanti;",
  "ja470:21.3_0": "Tato ahaṁ pabbajissāmi sakka,",
  "ja470:21.4_0": "Hitvāna kāmāni yathodhikānī”ti.",
  "ja470:22.1_0": "Kosiyajātakaṁ sattamaṁ."
}