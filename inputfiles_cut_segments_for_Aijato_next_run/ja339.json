{
  "ja339:0.1_0": "Jātaka",
  "ja339:0.2_0": "Catukkanipāta",
  "ja339:0.3_0": "Kokilavagga",
  "ja339:0.4_0": "9. Bāverujātaka",
  "ja339:1.1_0": "“Adassanena morassa,",
  "ja339:1.2_0": "sikhino mañjubhāṇino;",
  "ja339:1.3_0": "Kākaṁ tattha apūjesuṁ,",
  "ja339:1.4_0": "maṁsena ca phalena ca.",
  "ja339:2.1_0": "Yadā ca sarasampanno,",
  "ja339:2.2_0": "moro bāverumāgamā;",
  "ja339:2.3_0": "Atha lābho ca sakkāro,",
  "ja339:2.4_0": "vāyasassa ahāyatha.",
  "ja339:3.1_0": "Yāva nuppajjatī buddho,",
  "ja339:3.2_0": "dhammarājā pabhaṅkaro;",
  "ja339:3.3_0": "Tāva aññe apūjesuṁ,",
  "ja339:3.4_0": "puthū samaṇabrāhmaṇe.",
  "ja339:4.1_0": "Yadā ca sarasampanno,",
  "ja339:4.2_0": "buddho dhammaṁ adesayi;",
  "ja339:4.3_0": "Atha lābho ca sakkāro,",
  "ja339:4.4_0": "titthiyānaṁ ahāyathā”ti.",
  "ja339:5.1_0": "Bāverujātakaṁ navamaṁ."
}