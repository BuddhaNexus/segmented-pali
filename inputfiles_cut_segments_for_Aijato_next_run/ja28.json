{
  "ja28:0.1_0": "Jātaka",
  "ja28:0.2_0": "Ekakanipāta",
  "ja28:0.3_0": "Kuruṅgavagga",
  "ja28:0.4_0": "8. Nandivisālajātaka",
  "ja28:1.1_0": "“Manuññameva bhāseyya,",
  "ja28:1.2_0": "nāmanuññaṁ kudācanaṁ;",
  "ja28:1.3_0": "Manuññaṁ bhāsamānassa,",
  "ja28:1.4_0": "garuṁ bhāraṁ udaddhari;",
  "ja28:1.5_0": "Dhanañca naṁ alābhesi,",
  "ja28:1.6_0": "tena cattamano ahū”ti.",
  "ja28:2.1_0": "Nandivisālajātakaṁ aṭṭhamaṁ."
}