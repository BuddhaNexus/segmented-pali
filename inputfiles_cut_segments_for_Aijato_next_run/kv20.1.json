{
  "kv20.1:0.1_0": "Kathāvatthu",
  "kv20.1:0.2_0": "Niggahapaṇṇāsaka",
  "kv20.1:0.3_0": "Vīsatimavagga",
  "kv20.1:0.4_0": "Asañciccakathā",
  "kv20.1:1.1_0": "Asañcicca mātaraṁ jīvitā voropetvā ānantariko hotīti?",
  "kv20.1:1.2_0": "Āmantā.",
  "kv20.1:1.3_0": "Asañcicca pāṇaṁ hantvā pāṇātipātī hotīti?",
  "kv20.1:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv20.1:1.5_0": "asañcicca mātaraṁ jīvitā voropetvā ānantariko hotīti?",
  "kv20.1:1.6_0": "Āmantā.",
  "kv20.1:1.7_0": "Asañcicca adinnaṁ ādiyitvā …pe…",
  "kv20.1:1.8_0": "musā bhaṇitvā musāvādī hotīti?",
  "kv20.1:1.9_0": "Na hevaṁ vattabbe …pe….",
  "kv20.1:2.1_0": "Asañcicca pāṇaṁ hantvā pāṇātipātī na hotīti?",
  "kv20.1:2.2_0": "Āmantā.",
  "kv20.1:2.3_0": "Asañcicca mātaraṁ jīvitā voropetvā ānantariko na hotīti?",
  "kv20.1:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv20.1:2.5_0": "asañcicca adinnaṁ ādiyitvā …pe…",
  "kv20.1:2.6_0": "musā bhaṇitvā musāvādī na hotīti?",
  "kv20.1:2.7_0": "Āmantā.",
  "kv20.1:2.8_0": "Asañcicca mātaraṁ jīvitā voropetvā ānantariko na hotīti?",
  "kv20.1:2.9_0": "Na hevaṁ vattabbe …pe….",
  "kv20.1:3.1_0": "Asañcicca mātaraṁ jīvitā voropetvā ānantariko hotīti?",
  "kv20.1:3.2_0": "Āmantā.",
  "kv20.1:3.3_0": "“Asañcicca mātaraṁ jīvitā voropetvā ānantariko hotī”ti—",
  "kv20.1:3.4_0": "attheva suttantoti?",
  "kv20.1:3.5_0": "Natthi.",
  "kv20.1:3.6_0": "“Sañcicca mātaraṁ jīvitā voropetvā ānantariko hotī”ti—",
  "kv20.1:3.7_0": "attheva suttantoti?",
  "kv20.1:3.8_0": "Āmantā.",
  "kv20.1:3.9_0": "Hañci “sañcicca mātaraṁ jīvitā voropetvā ānantariko hotī”ti—",
  "kv20.1:3.10_0": "attheva suttanto, no ca vata re vattabbe—",
  "kv20.1:3.11_0": "“asañcicca mātaraṁ jīvitā voropetvā ānantariko hotī”ti.",
  "kv20.1:4.1_0": "Na vattabbaṁ—",
  "kv20.1:4.2_0": "“mātughātako ānantariko”ti?",
  "kv20.1:4.3_0": "Āmantā.",
  "kv20.1:4.4_0": "Nanu mātā jīvitā voropitāti?",
  "kv20.1:4.5_0": "Āmantā.",
  "kv20.1:4.6_0": "Hañci mātā jīvitā voropitā, tena vata re vattabbe—",
  "kv20.1:4.7_0": "“mātughātako ānantariko”ti.",
  "kv20.1:5.1_0": "Na vattabbaṁ—",
  "kv20.1:5.2_0": "“pitughātako ānantariko”ti?",
  "kv20.1:5.3_0": "Āmantā.",
  "kv20.1:5.4_0": "Nanu pitā jīvitā voropitoti?",
  "kv20.1:5.5_0": "Āmantā.",
  "kv20.1:5.6_0": "Hañci pitā jīvitā voropito, tena vata re vattabbe—",
  "kv20.1:5.7_0": "“pitughātako ānantariko”ti.",
  "kv20.1:6.1_0": "Na vattabbaṁ—",
  "kv20.1:6.2_0": "“arahantaghātako ānantariko”ti?",
  "kv20.1:6.3_0": "Āmantā.",
  "kv20.1:6.4_0": "Nanu arahā jīvitā voropitoti?",
  "kv20.1:6.5_0": "Āmantā.",
  "kv20.1:6.6_0": "Hañci arahā jīvitā voropito, tena vata re vattabbe—",
  "kv20.1:6.7_0": "“arahantaghātako ānantariko”ti.",
  "kv20.1:7.1_0": "Na vattabbaṁ—",
  "kv20.1:7.2_0": "“ruhiruppādako ānantariko”ti?",
  "kv20.1:7.3_0": "Āmantā.",
  "kv20.1:7.4_0": "Nanu tathāgatassa lohitaṁ uppāditanti?",
  "kv20.1:7.5_0": "Āmantā.",
  "kv20.1:7.6_0": "Hañci tathāgatassa lohitaṁ uppāditaṁ, tena vata re vattabbe—",
  "kv20.1:7.7_0": "“ruhiruppādako ānantariko”ti.",
  "kv20.1:8.1_0": "Saṅghabhedako ānantarikoti?",
  "kv20.1:8.2_0": "Āmantā.",
  "kv20.1:8.3_0": "Sabbe saṅghabhedakā ānantarikāti?",
  "kv20.1:8.4_0": "Na hevaṁ vattabbe …pe…",
  "kv20.1:8.5_0": "sabbe saṅghabhedakā ānantarikāti?",
  "kv20.1:8.6_0": "Āmantā.",
  "kv20.1:8.7_0": "Dhammasaññī saṅghabhedako ānantarikoti?",
  "kv20.1:8.8_0": "Na hevaṁ vattabbe …pe….",
  "kv20.1:9.1_0": "Dhammasaññī saṅghabhedako ānantarikoti?",
  "kv20.1:9.2_0": "Āmantā.",
  "kv20.1:9.3_0": "Nanu vuttaṁ bhagavatā—",
  "kv20.1:9.4_0": "“atthupāli, saṅghabhedako āpāyiko nerayiko kappaṭṭho atekiccho;",
  "kv20.1:9.5_0": "atthupāli, saṅghabhedako na āpāyiko na nerayiko na kappaṭṭho na atekiccho”ti.",
  "kv20.1:9.6_0": "Attheva suttantoti?",
  "kv20.1:9.7_0": "Āmantā.",
  "kv20.1:9.8_0": "Tena hi na vattabbaṁ—",
  "kv20.1:9.9_0": "“dhammasaññī saṅghabhedako ānantariko”ti.",
  "kv20.1:10.1_0": "Na vattabbaṁ—",
  "kv20.1:10.2_0": "“dhammasaññī saṅghabhedako ānantariko”ti?",
  "kv20.1:10.3_0": "Āmantā.",
  "kv20.1:10.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv20.1:11.1_0": "“Āpāyiko nerayiko,",
  "kv20.1:11.2_0": "Kappaṭṭho saṅghabhedako;",
  "kv20.1:11.3_0": "Vaggarato adhammaṭṭho,",
  "kv20.1:11.4_0": "Yogakkhemā padhaṁsati;",
  "kv20.1:11.5_0": "Saṅghaṁ samaggaṁ bhetvāna,",
  "kv20.1:11.6_0": "Kappaṁ nirayamhi paccatī”ti.",
  "kv20.1:12.1_0": "Attheva suttantoti?",
  "kv20.1:12.2_0": "Āmantā.",
  "kv20.1:12.3_0": "Tena hi saṅghabhedako ānantarikoti.",
  "kv20.1:12.4_0": "Asañciccakathā niṭṭhitā."
}