{
  "pp2.8:0.1_0": "Puggalapaññatti",
  "pp2.8:0.2_0": "Niddesa",
  "pp2.8:0.3_0": "8. Aṭṭhakapuggalapaññatti",
  "pp2.8:1.1_0": "Tattha katame cattāro maggasamaṅgino, cattāro phalasamaṅgino puggalā?",
  "pp2.8:1.2_0": "Sotāpanno, sotāpattiphalasacchikiriyāya paṭipanno;",
  "pp2.8:1.3_0": "sakadāgāmī, sakadāgāmiphalasacchikiriyāya paṭipanno;",
  "pp2.8:1.4_0": "anāgāmī, anāgāmiphalasacchikiriyāya paṭipanno;",
  "pp2.8:1.5_0": "arahā, arahattaphalasacchikiriyāya paṭipanno;",
  "pp2.8:1.6_0": "ime cattāro maggasamaṅgino, ime cattāro phalasamaṅgino puggalā.",
  "pp2.8:2.1_0": "Aṭṭhakaniddeso."
}