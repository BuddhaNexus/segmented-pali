{
  "ja298:0.1_0": "Jātaka",
  "ja298:0.2_0": "Tikanipāta",
  "ja298:0.3_0": "Kumbhavagga",
  "ja298:0.4_0": "8. Udumbarajātaka",
  "ja298:1.1_0": "“Udumbarā cime pakkā,",
  "ja298:1.2_0": "nigrodhā ca kapitthanā;",
  "ja298:1.3_0": "Ehi nikkhama bhuñjassu,",
  "ja298:1.4_0": "kiṁ jighacchāya miyyasi”.",
  "ja298:2.1_0": "“Evaṁ so suhito hoti,",
  "ja298:2.2_0": "yo vuḍḍhamapacāyati;",
  "ja298:2.3_0": "Yathāhamajja suhito,",
  "ja298:2.4_0": "dumapakkāni māsito”.",
  "ja298:3.1_0": "“Yaṁ vanejo vanejassa,",
  "ja298:3.2_0": "vañceyya kapino kapi;",
  "ja298:3.3_0": "Daharo kapi saddheyya,",
  "ja298:3.4_0": "na hi jiṇṇo jarākapī”ti.",
  "ja298:4.1_0": "Udumbarajātakaṁ aṭṭhamaṁ."
}