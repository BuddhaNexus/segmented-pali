{
  "tha-ap334:0.1_0": "Therāpadāna",
  "tha-ap334:0.2_0": "Gandhodakavagga",
  "tha-ap334:0.3_0": "2. Udakapūjakattheraapadāna",
  "tha-ap334:1.1_0": "“Suvaṇṇavaṇṇaṁ sambuddhaṁ,",
  "tha-ap334:1.2_0": "Gacchantaṁ anilañjase;",
  "tha-ap334:1.3_0": "Ghatāsanaṁva jalitaṁ,",
  "tha-ap334:1.4_0": "Ādittaṁva hutāsanaṁ.",
  "tha-ap334:2.1_0": "Pāṇinā udakaṁ gayha,",
  "tha-ap334:2.2_0": "ākāse ukkhipiṁ ahaṁ;",
  "tha-ap334:2.3_0": "Sampaṭicchi mahāvīro,",
  "tha-ap334:2.4_0": "buddho kāruṇiko isi.",
  "tha-ap334:3.1_0": "Antalikkhe ṭhito satthā,",
  "tha-ap334:3.2_0": "padumuttaranāmako;",
  "tha-ap334:3.3_0": "Mama saṅkappamaññāya,",
  "tha-ap334:3.4_0": "imaṁ gāthamabhāsatha.",
  "tha-ap334:4.1_0": "‘Iminā dakadānena,",
  "tha-ap334:4.2_0": "pītiuppādanena ca;",
  "tha-ap334:4.3_0": "Kappasatasahassampi,",
  "tha-ap334:4.4_0": "duggatiṁ nupapajjati’.",
  "tha-ap334:5.1_0": "Tena kammena dvipadinda,",
  "tha-ap334:5.2_0": "lokajeṭṭha narāsabha;",
  "tha-ap334:5.3_0": "Pattomhi acalaṁ ṭhānaṁ,",
  "tha-ap334:5.4_0": "hitvā jayaparājayaṁ.",
  "tha-ap334:6.1_0": "Sahassarājanāmena,",
  "tha-ap334:6.2_0": "tayo ca cakkavattino;",
  "tha-ap334:6.3_0": "Pañcasaṭṭhikappasate,",
  "tha-ap334:6.4_0": "cāturantā janādhipā.",
  "tha-ap334:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap334:7.2_0": "…pe…",
  "tha-ap334:7.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap334:8.1_0": "Itthaṁ sudaṁ āyasmā udakapūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap334:9.1_0": "Udakapūjakattherassāpadānaṁ dutiyaṁ."
}