{
  "ja302:0.1_0": "Jātaka",
  "ja302:0.2_0": "Catukkanipāta",
  "ja302:0.3_0": "Kāliṅgavagga",
  "ja302:0.4_0": "2. Mahāassārohajātaka",
  "ja302:1.1_0": "“Adeyyesu dadaṁ dānaṁ,",
  "ja302:1.2_0": "deyyesu nappavecchati;",
  "ja302:1.3_0": "Āpāsu byasanaṁ patto,",
  "ja302:1.4_0": "sahāyaṁ nādhigacchati.",
  "ja302:2.1_0": "Nādeyyesu dadaṁ dānaṁ,",
  "ja302:2.2_0": "deyyesu yo pavecchati;",
  "ja302:2.3_0": "Āpāsu byasanaṁ patto,",
  "ja302:2.4_0": "sahāyamadhigacchati.",
  "ja302:3.1_0": "Saññogasambhogavisesadassanaṁ,",
  "ja302:3.2_0": "Anariyadhammesu saṭhesu nassati;",
  "ja302:3.3_0": "Katañca ariyesu ca ajjavesu,",
  "ja302:3.4_0": "Mahapphalaṁ hoti aṇumpi tādisu.",
  "ja302:4.1_0": "Yo pubbe katakalyāṇo,",
  "ja302:4.2_0": "Akā loke sudukkaraṁ;",
  "ja302:4.3_0": "Pacchā kayirā na vā kayirā,",
  "ja302:4.4_0": "Accantaṁ pūjanāraho”ti.",
  "ja302:5.1_0": "Mahāassārohajātakaṁ dutiyaṁ."
}