{
  "iti55:0.1_0": "Itivuttaka 55",
  "iti55:0.2_0": "Tikanipāta",
  "iti55:0.3_0": "Paṭhamavagga",
  "iti55:0.4_0": "Dutiyaesanāsutta",
  "iti55:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti55:2.1_0": "“Tisso imā, bhikkhave, esanā.",
  "iti55:2.2_0": "Katamā tisso?",
  "iti55:2.3_0": "Kāmesanā, bhavesanā, brahmacariyesanā—",
  "iti55:2.4_0": "imā kho, bhikkhave, tisso esanā”ti.",
  "iti55:2.5_0": "Etamatthaṁ bhagavā avoca.",
  "iti55:2.6_0": "Tatthetaṁ iti vuccati:",
  "iti55:3.1_0": "“Kāmesanā bhavesanā,",
  "iti55:3.2_0": "brahmacariyesanā saha;",
  "iti55:3.3_0": "Iti saccaparāmāso,",
  "iti55:3.4_0": "diṭṭhiṭṭhānā samussayā.",
  "iti55:4.1_0": "Sabbarāgavirattassa,",
  "iti55:4.2_0": "taṇhakkhayavimuttino;",
  "iti55:4.3_0": "Esanā paṭinissaṭṭhā,",
  "iti55:4.4_0": "diṭṭhiṭṭhānā samūhatā;",
  "iti55:4.5_0": "Esanānaṁ khayā bhikkhu,",
  "iti55:4.6_0": "nirāso akathaṅkathī”ti.",
  "iti55:5.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti55:5.2_0": "Chaṭṭhaṁ."
}