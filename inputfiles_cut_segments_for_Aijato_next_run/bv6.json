{
  "bv6:0.1_0": "Buddhavaṁsa",
  "bv6:0.2_0": "6. Sumanabuddhavaṁsa",
  "bv6:1.1_0": "“Maṅgalassa aparena,",
  "bv6:1.2_0": "sumano nāma nāyako;",
  "bv6:1.3_0": "Sabbadhammehi asamo,",
  "bv6:1.4_0": "sabbasattānamuttamo.",
  "bv6:2.1_0": "Tadā amatabheriṁ so,",
  "bv6:2.2_0": "āhanī mekhale pure;",
  "bv6:2.3_0": "Dhammasaṅkhasamāyuttaṁ,",
  "bv6:2.4_0": "navaṅgaṁ jinasāsanaṁ.",
  "bv6:3.1_0": "Nijjinitvā kilese so,",
  "bv6:3.2_0": "patvā sambodhimuttamaṁ;",
  "bv6:3.3_0": "Māpesi nagaraṁ satthā,",
  "bv6:3.4_0": "saddhammapuravaruttamaṁ.",
  "bv6:4.1_0": "Nirantaraṁ akuṭilaṁ,",
  "bv6:4.2_0": "ujuṁ vipulavitthataṁ;",
  "bv6:4.3_0": "Māpesi so mahāvīthiṁ,",
  "bv6:4.4_0": "satipaṭṭhānavaruttamaṁ.",
  "bv6:5.1_0": "Phale cattāri sāmaññe,",
  "bv6:5.2_0": "catasso paṭisambhidā;",
  "bv6:5.3_0": "Chaḷabhiññāṭṭhasamāpattī,",
  "bv6:5.4_0": "pasāresi tattha vīthiyaṁ.",
  "bv6:6.1_0": "Ye appamattā akhilā,",
  "bv6:6.2_0": "hirivīriyehupāgatā;",
  "bv6:6.3_0": "Te te ime guṇavare,",
  "bv6:6.4_0": "ādiyanti yathā sukhaṁ.",
  "bv6:7.1_0": "Evametena yogena,",
  "bv6:7.2_0": "uddharanto mahājanaṁ;",
  "bv6:7.3_0": "Bodhesi paṭhamaṁ satthā,",
  "bv6:7.4_0": "koṭisatasahassiyo.",
  "bv6:8.1_0": "Yamhi kāle mahāvīro,",
  "bv6:8.2_0": "ovadī titthiye gaṇe;",
  "bv6:8.3_0": "Koṭisahassābhisamiṁsu,",
  "bv6:8.4_0": "dutiye dhammadesane.",
  "bv6:9.1_0": "Yadā devā manussā ca,",
  "bv6:9.2_0": "samaggā ekamānasā;",
  "bv6:9.3_0": "Nirodhapañhaṁ pucchiṁsu,",
  "bv6:9.4_0": "saṁsayañcāpi mānasaṁ.",
  "bv6:10.1_0": "Tadāpi dhammadesane,",
  "bv6:10.2_0": "nirodhaparidīpane;",
  "bv6:10.3_0": "Navutikoṭisahassānaṁ,",
  "bv6:10.4_0": "tatiyābhisamayo ahu.",
  "bv6:11.1_0": "Sannipātā tayo āsuṁ,",
  "bv6:11.2_0": "sumanassa mahesino;",
  "bv6:11.3_0": "Khīṇāsavānaṁ vimalānaṁ,",
  "bv6:11.4_0": "santacittāna tādinaṁ.",
  "bv6:12.1_0": "Vassaṁvutthassa bhagavato,",
  "bv6:12.2_0": "abhighuṭṭhe pavāraṇe;",
  "bv6:12.3_0": "Koṭisatasahassehi,",
  "bv6:12.4_0": "pavāresi tathāgato.",
  "bv6:13.1_0": "Tatoparaṁ sannipāte,",
  "bv6:13.2_0": "vimale kañcanapabbate;",
  "bv6:13.3_0": "Navutikoṭisahassānaṁ,",
  "bv6:13.4_0": "dutiyo āsi samāgamo.",
  "bv6:14.1_0": "Yadā sakko devarājā,",
  "bv6:14.2_0": "buddhadassanupāgami;",
  "bv6:14.3_0": "Asītikoṭisahassānaṁ,",
  "bv6:14.4_0": "tatiyo āsi samāgamo.",
  "bv6:15.1_0": "Ahaṁ tena samayena,",
  "bv6:15.2_0": "nāgarājā mahiddhiko;",
  "bv6:15.3_0": "Atulo nāma nāmena,",
  "bv6:15.4_0": "ussannakusalasañcayo.",
  "bv6:16.1_0": "Tadāhaṁ nāgabhavanā,",
  "bv6:16.2_0": "nikkhamitvā sañātibhi;",
  "bv6:16.3_0": "Nāgānaṁ dibbaturiyehi,",
  "bv6:16.4_0": "sasaṅghaṁ jinamupaṭṭhahiṁ.",
  "bv6:17.1_0": "Koṭisatasahassānaṁ,",
  "bv6:17.2_0": "annapānena tappayiṁ;",
  "bv6:17.3_0": "Paccekadussayugaṁ datvā,",
  "bv6:17.4_0": "saraṇaṁ tamupāgamiṁ.",
  "bv6:18.1_0": "Sopi maṁ buddho byākāsi,",
  "bv6:18.2_0": "sumano lokanāyako;",
  "bv6:18.3_0": "‘Aparimeyyito kappe,",
  "bv6:18.4_0": "ayaṁ buddho bhavissati.",
  "bv6:19.1_0": "Padhānaṁ padahitvāna,",
  "bv6:19.2_0": "…pe…",
  "bv6:19.3_0": "hessāma sammukhā imaṁ’.",
  "bv6:20.1_0": "Tassāpi vacanaṁ sutvā,",
  "bv6:20.2_0": "bhiyyo cittaṁ pasādayiṁ;",
  "bv6:20.3_0": "Uttariṁ vatamadhiṭṭhāsiṁ,",
  "bv6:20.4_0": "dasapāramipūriyā.",
  "bv6:21.1_0": "Nagaraṁ mekhalaṁ nāma,",
  "bv6:21.2_0": "sudatto nāma khattiyo;",
  "bv6:21.3_0": "Sirimā nāma janikā,",
  "bv6:21.4_0": "sumanassa mahesino.",
  "bv6:22.1_0": "Navavassasahassāni,",
  "bv6:22.2_0": "agāraṁ ajjha so vasi;",
  "bv6:22.3_0": "Cando sucando vaṭaṁso ca,",
  "bv6:22.4_0": "tayo pāsādamuttamā.",
  "bv6:23.1_0": "Tesaṭṭhisatasahassāni,",
  "bv6:23.2_0": "nāriyo samalaṅkatā;",
  "bv6:23.3_0": "Vaṭaṁsikā nāma nārī,",
  "bv6:23.4_0": "anūpamo nāma atrajo.",
  "bv6:24.1_0": "Nimitte caturo disvā,",
  "bv6:24.2_0": "hatthiyānena nikkhami;",
  "bv6:24.3_0": "Anūnadasamāsāni,",
  "bv6:24.4_0": "padhānaṁ padahī jino.",
  "bv6:25.1_0": "Brahmunā yācito santo,",
  "bv6:25.2_0": "sumano lokanāyako;",
  "bv6:25.3_0": "Vatti cakkaṁ mahāvīro,",
  "bv6:25.4_0": "mekhale puramuttame.",
  "bv6:26.1_0": "Saraṇo bhāvitatto ca,",
  "bv6:26.2_0": "ahesuṁ aggasāvakā;",
  "bv6:26.3_0": "Udeno nāmupaṭṭhāko,",
  "bv6:26.4_0": "sumanassa mahesino.",
  "bv6:27.1_0": "Soṇā ca upasoṇā ca,",
  "bv6:27.2_0": "ahesuṁ aggasāvikā;",
  "bv6:27.3_0": "Sopi buddho amitayaso,",
  "bv6:27.4_0": "nāgamūle abujjhatha.",
  "bv6:28.1_0": "Varuṇo ceva saraṇo ca,",
  "bv6:28.2_0": "ahesuṁ aggupaṭṭhakā;",
  "bv6:28.3_0": "Cālā ca upacālā ca,",
  "bv6:28.4_0": "ahesuṁ aggupaṭṭhikā.",
  "bv6:29.1_0": "Uccattanena so buddho,",
  "bv6:29.2_0": "navutihatthamuggato;",
  "bv6:29.3_0": "Kañcanagghiyasaṅkāso,",
  "bv6:29.4_0": "dasasahassī virocati.",
  "bv6:30.1_0": "Navutivassasahassāni,",
  "bv6:30.2_0": "āyu vijjati tāvade;",
  "bv6:30.3_0": "Tāvatā tiṭṭhamāno so,",
  "bv6:30.4_0": "tāresi janataṁ bahuṁ.",
  "bv6:31.1_0": "Tāraṇīye tārayitvā,",
  "bv6:31.2_0": "bodhanīye ca bodhayi;",
  "bv6:31.3_0": "Parinibbāyi sambuddho,",
  "bv6:31.4_0": "uḷurājāva atthami.",
  "bv6:32.1_0": "Te ca khīṇāsavā bhikkhū,",
  "bv6:32.2_0": "so ca buddho asādiso;",
  "bv6:32.3_0": "Atulappabhaṁ dassayitvā,",
  "bv6:32.4_0": "nibbutā ye mahāyasā.",
  "bv6:33.1_0": "Tañca ñāṇaṁ atuliyaṁ,",
  "bv6:33.2_0": "Tāni ca atulāni ratanāni;",
  "bv6:33.3_0": "Sabbaṁ tamantarahitaṁ,",
  "bv6:33.4_0": "Nanu rittā sabbasaṅkhārā.",
  "bv6:34.1_0": "Sumano yasadharo buddho,",
  "bv6:34.2_0": "Aṅgārāmamhi nibbuto;",
  "bv6:34.3_0": "Tattheva tassa jinathūpo,",
  "bv6:34.4_0": "Catuyojanamuggato”ti.",
  "bv6:35.1_0": "Sumanassa bhagavato vaṁso catuttho."
}