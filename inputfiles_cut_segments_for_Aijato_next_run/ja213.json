{
  "ja213:0.1_0": "Jātaka",
  "ja213:0.2_0": "Dukanipāta",
  "ja213:0.3_0": "Bīraṇathambhavagga",
  "ja213:0.4_0": "3. Bharujātaka",
  "ja213:1.1_0": "“Isīnamantaraṁ katvā,",
  "ja213:1.2_0": "bharurājāti me sutaṁ;",
  "ja213:1.3_0": "Ucchinno saha raṭṭhehi,",
  "ja213:1.4_0": "sarājā vibhavaṅgato.",
  "ja213:2.1_0": "Tasmā hi chandāgamanaṁ,",
  "ja213:2.2_0": "nappasaṁsanti paṇḍitā;",
  "ja213:2.3_0": "Aduṭṭhacitto bhāseyya,",
  "ja213:2.4_0": "giraṁ saccupasaṁhitan”ti.",
  "ja213:3.1_0": "Bharujātakaṁ tatiyaṁ."
}