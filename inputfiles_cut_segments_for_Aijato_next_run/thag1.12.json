{
  "thag1.12:0.1_0": "Theragāthā",
  "thag1.12:0.2_0": "Ekakanipāta",
  "thag1.12:0.3_0": "Dutiyavagga",
  "thag1.12:0.4_0": "1.12. Mahāvacchattheragāthā",
  "thag1.12:1.1_0": "“Paññābalī sīlavatūpapanno,",
  "thag1.12:1.2_0": "Samāhito jhānarato satīmā;",
  "thag1.12:1.3_0": "Yadatthiyaṁ bhojanaṁ bhuñjamāno,",
  "thag1.12:1.4_0": "Kaṅkhetha kālaṁ idha vītarāgo”ti.",
  "thag1.12:1.5_0": "… Mahāvaccho thero …."
}