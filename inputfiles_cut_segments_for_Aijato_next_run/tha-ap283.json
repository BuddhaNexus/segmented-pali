{
  "tha-ap283:0.1_0": "Therāpadāna",
  "tha-ap283:0.2_0": "Paṇṇadāyakavagga",
  "tha-ap283:0.3_0": "1. Paṇṇadāyakattheraapadāna",
  "tha-ap283:1.1_0": "“Paṇṇasāle nisinnomhi,",
  "tha-ap283:1.2_0": "paṇṇabhojanabhojano;",
  "tha-ap283:1.3_0": "Upaviṭṭhañca maṁ santaṁ,",
  "tha-ap283:1.4_0": "upāgacchi mahāisi.",
  "tha-ap283:2.1_0": "Siddhattho lokapajjoto,",
  "tha-ap283:2.2_0": "sabbalokatikicchako;",
  "tha-ap283:2.3_0": "Tassa paṇṇaṁ mayā dinnaṁ,",
  "tha-ap283:2.4_0": "nisinnaṁ paṇṇasanthare.",
  "tha-ap283:3.1_0": "Catunnavutito kappe,",
  "tha-ap283:3.2_0": "yaṁ paṇṇamadadiṁ tadā;",
  "tha-ap283:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap283:3.4_0": "paṇṇadānassidaṁ phalaṁ.",
  "tha-ap283:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap283:4.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap283:4.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap283:4.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap283:5.1_0": "Itthaṁ sudaṁ āyasmā paṇṇadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap283:6.1_0": "Paṇṇadāyakattherassāpadānaṁ paṭhamaṁ."
}