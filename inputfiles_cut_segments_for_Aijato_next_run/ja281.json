{
  "ja281:0.1_0": "Jātaka",
  "ja281:0.2_0": "Tikanipāta",
  "ja281:0.3_0": "Abbhantaravagga",
  "ja281:0.4_0": "1. Abbhantarajātaka",
  "ja281:1.1_0": "“Abbhantaro nāma dumo,",
  "ja281:1.2_0": "yassa dibyamidaṁ phalaṁ;",
  "ja281:1.3_0": "Bhutvā dohaḷinī nārī,",
  "ja281:1.4_0": "cakkavattiṁ vijāyati.",
  "ja281:2.1_0": "Tvampi bhadde mahesīsi,",
  "ja281:2.2_0": "sā cāpi patino piyā;",
  "ja281:2.3_0": "Āharissati te rājā,",
  "ja281:2.4_0": "idaṁ abbhantaraṁ phalaṁ”.",
  "ja281:3.1_0": "“Bhatturatthe parakkanto,",
  "ja281:3.2_0": "yaṁ ṭhānamadhigacchati;",
  "ja281:3.3_0": "Sūro attapariccāgī,",
  "ja281:3.4_0": "labhamāno bhavāmahan”ti.",
  "ja281:4.1_0": "Abbhantarajātakaṁ paṭhamaṁ."
}