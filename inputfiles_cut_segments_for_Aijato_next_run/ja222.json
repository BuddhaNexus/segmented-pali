{
  "ja222:0.1_0": "Jātaka",
  "ja222:0.2_0": "Dukanipāta",
  "ja222:0.3_0": "Kāsāvavagga",
  "ja222:0.4_0": "2. Cūḷanandiyajātaka",
  "ja222:1.1_0": "“Idaṁ tadācariyavaco,",
  "ja222:1.2_0": "pārāsariyo yadabravi;",
  "ja222:1.3_0": "Māsu tvaṁ akari pāpaṁ,",
  "ja222:1.4_0": "yaṁ tvaṁ pacchā kataṁ tape.",
  "ja222:2.1_0": "Yāni karoti puriso,",
  "ja222:2.2_0": "tāni attani passati;",
  "ja222:2.3_0": "Kalyāṇakārī kalyāṇaṁ,",
  "ja222:2.4_0": "pāpakārī ca pāpakaṁ;",
  "ja222:2.5_0": "Yādisaṁ vapate bījaṁ,",
  "ja222:2.6_0": "tādisaṁ harate phalan”ti.",
  "ja222:3.1_0": "Cūḷanandiyajātakaṁ dutiyaṁ."
}