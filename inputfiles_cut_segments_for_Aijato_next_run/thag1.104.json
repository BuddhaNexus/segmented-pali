{
  "thag1.104:0.1_0": "Theragāthā",
  "thag1.104:0.2_0": "Ekakanipāta",
  "thag1.104:0.3_0": "Ekādasamavagga",
  "thag1.104:0.4_0": "1.104. Khitakattheragāthā",
  "thag1.104:1.1_0": "“Lahuko vata me kāyo,",
  "thag1.104:1.2_0": "Phuṭṭho ca pītisukhena vipulena;",
  "thag1.104:1.3_0": "Tūlamiva eritaṁ mālutena,",
  "thag1.104:1.4_0": "Pilavatīva me kāyo”ti.",
  "thag1.104:1.5_0": "… Khitako thero …."
}