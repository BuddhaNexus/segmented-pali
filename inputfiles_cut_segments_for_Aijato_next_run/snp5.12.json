{
  "snp5.12:0.1_0": "Sutta Nipāta 5.12",
  "snp5.12:0.2_0": "Jatukaṇṇimāṇavapucchā",
  "snp5.12:1.1_0": "“Sutvānahaṁ vīramakāmakāmiṁ,",
  "snp5.12:1.2_0": "(iccāyasmā jatukaṇṇi)",
  "snp5.12:1.3_0": "Oghātigaṁ puṭṭhumakāmamāgamaṁ;",
  "snp5.12:1.4_0": "Santipadaṁ brūhi sahajanetta,",
  "snp5.12:1.5_0": "Yathātacchaṁ bhagavā brūhi metaṁ.",
  "snp5.12:2.1_0": "Bhagavā hi kāme abhibhuyya iriyati,",
  "snp5.12:2.2_0": "Ādiccova pathaviṁ tejī tejasā;",
  "snp5.12:2.3_0": "Parittapaññassa me bhūripañña,",
  "snp5.12:2.4_0": "Ācikkha dhammaṁ yamahaṁ vijaññaṁ;",
  "snp5.12:2.5_0": "Jātijarāya idha vippahānaṁ”.",
  "snp5.12:3.1_0": "“Kāmesu vinaya gedhaṁ,",
  "snp5.12:3.2_0": "(jatukaṇṇīti bhagavā)",
  "snp5.12:3.3_0": "Nekkhammaṁ daṭṭhu khemato;",
  "snp5.12:3.4_0": "Uggahītaṁ nirattaṁ vā,",
  "snp5.12:3.5_0": "Mā te vijjittha kiñcanaṁ.",
  "snp5.12:4.1_0": "Yaṁ pubbe taṁ visosehi,",
  "snp5.12:4.2_0": "pacchā te māhu kiñcanaṁ;",
  "snp5.12:4.3_0": "Majjhe ce no gahessasi,",
  "snp5.12:4.4_0": "upasanto carissasi.",
  "snp5.12:5.1_0": "Sabbaso nāmarūpasmiṁ,",
  "snp5.12:5.2_0": "Vītagedhassa brāhmaṇa;",
  "snp5.12:5.3_0": "Āsavāssa na vijjanti,",
  "snp5.12:5.4_0": "Yehi maccuvasaṁ vaje”ti.",
  "snp5.12:5.5_0": "Jatukaṇṇimāṇavapucchā ekādasamā."
}