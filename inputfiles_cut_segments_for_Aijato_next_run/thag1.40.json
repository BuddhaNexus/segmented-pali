{
  "thag1.40:0.1_0": "Theragāthā",
  "thag1.40:0.2_0": "Ekakanipāta",
  "thag1.40:0.3_0": "Catutthavagga",
  "thag1.40:0.4_0": "1.40. Vaḍḍhamānattheragāthā",
  "thag1.40:1.1_0": "“Sattiyā viya omaṭṭho,",
  "thag1.40:1.2_0": "ḍayhamānova matthake;",
  "thag1.40:1.3_0": "Bhavarāgappahānāya,",
  "thag1.40:1.4_0": "sato bhikkhu paribbaje”ti.",
  "thag1.40:1.5_0": "…",
  "thag1.40:1.6_0": "Vaḍḍhamāno thero ….",
  "thag1.40:1.7_0": "vaggo catuttho.",
  "thag1.40:2.0_0": "Tassuddānaṁ",
  "thag1.40:2.1_0": "Gahvaratīriyo suppiyo,",
  "thag1.40:2.2_0": "sopāko ceva posiyo;",
  "thag1.40:2.3_0": "Sāmaññakāni kumāputto,",
  "thag1.40:2.4_0": "kumāputtasahāyako;",
  "thag1.40:2.5_0": "Gavampati tissatthero,",
  "thag1.40:2.6_0": "vaḍḍhamāno mahāyasoti."
}