{
  "tika-abh06t53:0_0": "Dvādasamo paricchedo",
  "tika-abh06t53:1_0": "12. Rāsivinicchayakathā",
  "tika-abh06t53:2_0": "416.",
  "tika-abh06t53:3_0": "Tattha viññāṇakāyā cha, satta viññāṇadhātuyo;",
  "tika-abh06t53:4_0": "Phassā cakkhādisamphassā, chabbidhā sattadhāpi ca.",
  "tika-abh06t53:5_0": "417.",
  "tika-abh06t53:6_0": "Cakkhusamphassajādīhi, bhedehi pana vedanā;",
  "tika-abh06t53:7_0": "Saññā ca cetanā ceva, bhinnā chadhā ca sattadhā.",
  "tika-abh06t53:8_0": "418.",
  "tika-abh06t53:9_0": "Cittuppādesu dhammā ca, khandhāyatanadhātuyo;",
  "tika-abh06t53:10_0": "Āhārā ca yathāyogaṁ, phassapañcakarāsiyaṁ.",
  "tika-abh06t53:11_0": "419.",
  "tika-abh06t53:12_0": "Sabbe saṅgahitā honti, tasmā nāmapariggaho;",
  "tika-abh06t53:13_0": "Mūlarāsi ca so sabba-saṅgahoti pavuccati.",
  "tika-abh06t53:14_0": "420.",
  "tika-abh06t53:15_0": "Jhānarāsimhi pañceva, dhammā sattappabhedato;",
  "tika-abh06t53:16_0": "Indriyāni ca bāvīsa, dhammato pana soḷasa.",
  "tika-abh06t53:17_0": "421.",
  "tika-abh06t53:18_0": "Nava maggaṅgadhammā ca, bhinnā dvādasadhāpi te;",
  "tika-abh06t53:19_0": "Chaḷeva hetuyo tattha, desitā kaṅkhituddhavā.",
  "tika-abh06t53:20_0": "422.",
  "tika-abh06t53:21_0": "Dasa kammapathā dhammā, chaḷeva pana desitā;",
  "tika-abh06t53:22_0": "Sesāva dasadhammehi, samānā caturāsayo.",
  "tika-abh06t53:23_0": "423.",
  "tika-abh06t53:24_0": "Paññā dasavidhā tattha, vedanā navadhā ṭhitā;",
  "tika-abh06t53:25_0": "Samādhi sattadhā hoti, vīriyaṁ pana pañcadhā.",
  "tika-abh06t53:26_0": "424.",
  "tika-abh06t53:27_0": "Sati bhinnā catudhāva, vitakko tividho mato;",
  "tika-abh06t53:28_0": "Dvidhā cittādayo honti, dasapañceva sambhavā.",
  "tika-abh06t53:29_0": "425.",
  "tika-abh06t53:30_0": "Sesā dvattiṁsa sabbepi, dhammā ekekadhāpi ca;",
  "tika-abh06t53:31_0": "Hitvā rūpindriyānete, vibhāgā aṭṭhadhā kathaṁ.",
  "tika-abh06t53:32_0": "426.",
  "tika-abh06t53:33_0": "Phasso ca cetanā saññā, vicāro pīti jīvitaṁ;",
  "tika-abh06t53:34_0": "Niravajjā cha yugaḷā, sāvajjamohakaṅkhitā.",
  "tika-abh06t53:35_0": "427.",
  "tika-abh06t53:36_0": "Yevāpanakadhammā ca, viratuddhaccavajjitā;",
  "tika-abh06t53:37_0": "Dvādasā ceti sabbepi, dvattiṁsakekadhā tathā.",
  "tika-abh06t53:38_0": "428.",
  "tika-abh06t53:39_0": "Cittaṁ manindriyaṁ cittaṁ, saddhā saddhindriyaṁ balaṁ;",
  "tika-abh06t53:40_0": "Balesu lokiyā vuttā, lokiye ca dukadvaye.",
  "tika-abh06t53:41_0": "429.",
  "tika-abh06t53:42_0": "Lobhālobhādikā dve dve,",
  "tika-abh06t53:43_0": "Cattāro heturāsiyaṁ;",
  "tika-abh06t53:44_0": "Micchādiṭṭhi ca maggaṅge,",
  "tika-abh06t53:45_0": "Pañcakammapathepi te.",
  "tika-abh06t53:46_0": "430.",
  "tika-abh06t53:47_0": "Yevāpanakarāsimhi, desitā viratuddhavā;",
  "tika-abh06t53:48_0": "Maggahetūsu ceveti, dvidhā pañcadasa ṭhitā.",
  "tika-abh06t53:49_0": "431.",
  "tika-abh06t53:50_0": "Vitakko jhānamaggesu, tividhā navadhā pana;",
  "tika-abh06t53:51_0": "Vedanā mūlarāsimhi, tathā jhānindriyesu ca.",
  "tika-abh06t53:52_0": "432.",
  "tika-abh06t53:53_0": "Indriyamaggarāsimhi, balapiṭṭhidukattike;",
  "tika-abh06t53:54_0": "Catudhā sati tattheva, vīriyampi ca pañcadhā.",
  "tika-abh06t53:55_0": "433.",
  "tika-abh06t53:56_0": "Samādhi sattadhā vutto, jhānaṅgesu ca tattha ca;",
  "tika-abh06t53:57_0": "Tattheva dasadhā paññā, hetukammapathesu ca.",
  "tika-abh06t53:58_0": "434.",
  "tika-abh06t53:59_0": "Dasanavasattapañcacatutidvekadhā ṭhitā;",
  "tika-abh06t53:60_0": "Chaḷekakā pañcadasa, dvattiṁsa ca yathākkamaṁ.",
  "tika-abh06t53:61_0": "435.",
  "tika-abh06t53:62_0": "Aṭṭha vibhāgasaṅkhepā, padāni dasadhā siyuṁ;",
  "tika-abh06t53:63_0": "Tepaññāseva dhammā ca, aṭṭhārasa ca rāsayo.",
  "tika-abh06t53:64_0": "436.",
  "tika-abh06t53:65_0": "Iti dhammavavatthāne, dhammasaṅgaṇiyaṁ pana;",
  "tika-abh06t53:66_0": "Cittuppādaparicchede, uddesanayasaṅgaho.",
  "tika-abh06t53:67_0": "437.",
  "tika-abh06t53:68_0": "Padāni caturāsīti, desitāni sarūpato;",
  "tika-abh06t53:69_0": "Yevāpanakanāmena, soḷaseva yathārahaṁ.",
  "tika-abh06t53:70_0": "438.",
  "tika-abh06t53:71_0": "Tatthāniyatanāmāni, padānekādaseva tu;",
  "tika-abh06t53:72_0": "Vuttānekūnanavuti, niyatāneva sambhavā.",
  "tika-abh06t53:73_0": "439.",
  "tika-abh06t53:74_0": "Asambhinnapadānettha, tepaññāseva sabbathā;",
  "tika-abh06t53:75_0": "Cittacetasikānaṁ tu, vasena paridīpaye.",
  "tika-abh06t53:76_0": "440.",
  "tika-abh06t53:77_0": "Vibhāgapadadhammānaṁ, vasenevaṁ pakāsito;",
  "tika-abh06t53:78_0": "Cittacetasikānaṁ tu, kamato rāsinicchayoti.",
  "tika-abh06t53:79_0": "Iti cetasikavibhāge rāsivinicchayakathā niṭṭhitā.",
  "tika-abh06t53:80_0": "Dvādasamo paricchedo."
}