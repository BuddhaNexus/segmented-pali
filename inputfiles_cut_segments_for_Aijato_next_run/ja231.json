{
  "ja231:0.1_0": "Jātaka",
  "ja231:0.2_0": "Dukanipāta",
  "ja231:0.3_0": "Upāhanavagga",
  "ja231:0.4_0": "1. Upāhanajātaka",
  "ja231:1.1_0": "“Yathāpi kītā purisassupāhanā,",
  "ja231:1.2_0": "Sukhassa atthāya dukhaṁ udabbahe;",
  "ja231:1.3_0": "Ghammābhitattā thalasā papīḷitā,",
  "ja231:1.4_0": "Tasseva pāde purisassa khādare.",
  "ja231:2.1_0": "Evameva yo dukkulīno anariyo,",
  "ja231:2.2_0": "Tammāka vijjañca sutañca ādiya;",
  "ja231:2.3_0": "Tameva so tattha sutena khādati,",
  "ja231:2.4_0": "Anariyo vuccati dupāhanūpamo”ti.",
  "ja231:3.1_0": "Upāhanajātakaṁ paṭhamaṁ."
}