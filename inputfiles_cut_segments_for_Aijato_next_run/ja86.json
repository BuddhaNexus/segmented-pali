{
  "ja86:0.1_0": "Jātaka",
  "ja86:0.2_0": "Ekakanipāta",
  "ja86:0.3_0": "Apāyimhavagga",
  "ja86:0.4_0": "6. Sīlavīmaṁsakajātaka",
  "ja86:1.1_0": "“Sīlaṁ kireva kalyāṇaṁ,",
  "ja86:1.2_0": "sīlaṁ loke anuttaraṁ;",
  "ja86:1.3_0": "Passa ghoraviso nāgo,",
  "ja86:1.4_0": "sīlavāti na haññatī”ti.",
  "ja86:2.1_0": "Sīlavīmaṁsakajātakaṁ chaṭṭhaṁ."
}