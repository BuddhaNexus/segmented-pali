{
  "thag1.47:0.1_0": "Theragāthā",
  "thag1.47:0.2_0": "Ekakanipāta",
  "thag1.47:0.3_0": "Pañcamavagga",
  "thag1.47:0.4_0": "1.47. Ujjayattheragāthā",
  "thag1.47:1.1_0": "“Namo te buddha vīratthu,",
  "thag1.47:1.2_0": "vippamuttosi sabbadhi;",
  "thag1.47:1.3_0": "Tuyhāpadāne viharaṁ,",
  "thag1.47:1.4_0": "viharāmi anāsavo”ti.",
  "thag1.47:1.5_0": "… Ujjayo thero …."
}