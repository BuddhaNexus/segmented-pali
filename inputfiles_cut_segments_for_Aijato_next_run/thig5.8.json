{
  "thig5.8:0.1_0": "Therīgāthā",
  "thig5.8:0.2_0": "Pañcakanipāta",
  "thig5.8:0.3_0": "8. Soṇātherīgāthā",
  "thig5.8:1.1_0": "“Dasa putte vijāyitvā,",
  "thig5.8:1.2_0": "asmiṁ rūpasamussaye;",
  "thig5.8:1.3_0": "Tatohaṁ dubbalā jiṇṇā,",
  "thig5.8:1.4_0": "bhikkhuniṁ upasaṅkamiṁ.",
  "thig5.8:2.1_0": "Sā me dhammamadesesi,",
  "thig5.8:2.2_0": "khandhāyatanadhātuyo;",
  "thig5.8:2.3_0": "Tassā dhammaṁ suṇitvāna,",
  "thig5.8:2.4_0": "kese chetvāna pabbajiṁ.",
  "thig5.8:3.1_0": "Tassā me sikkhamānāya,",
  "thig5.8:3.2_0": "dibbacakkhu visodhitaṁ;",
  "thig5.8:3.3_0": "Pubbenivāsaṁ jānāmi,",
  "thig5.8:3.4_0": "yattha me vusitaṁ pure.",
  "thig5.8:4.1_0": "Animittañca bhāvemi,",
  "thig5.8:4.2_0": "ekaggā susamāhitā;",
  "thig5.8:4.3_0": "Anantarāvimokkhāsiṁ,",
  "thig5.8:4.4_0": "anupādāya nibbutā.",
  "thig5.8:5.1_0": "Pañcakkhandhā pariññātā,",
  "thig5.8:5.2_0": "tiṭṭhanti chinnamūlakā;",
  "thig5.8:5.3_0": "Dhi tavatthu jare jamme,",
  "thig5.8:5.4_0": "natthi dāni punabbhavo”ti.",
  "thig5.8:6.1_0": "… Soṇā therī …."
}