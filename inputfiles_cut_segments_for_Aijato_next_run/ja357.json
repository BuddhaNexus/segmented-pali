{
  "ja357:0.1_0": "Jātaka",
  "ja357:0.2_0": "Pañcakanipāta",
  "ja357:0.3_0": "Maṇikuṇḍalavagga",
  "ja357:0.4_0": "7. Laṭukikajātaka",
  "ja357:1.1_0": "“Vandāmi taṁ kuñjara saṭṭhihāyanaṁ,",
  "ja357:1.2_0": "Āraññakaṁ yūthapatiṁ yasassiṁ;",
  "ja357:1.3_0": "Pakkhehi taṁ pañjalikaṁ karomi,",
  "ja357:1.4_0": "Mā me vadhī puttake dubbalāya”.",
  "ja357:2.1_0": "“Vandāmi taṁ kuñjara ekacāriṁ,",
  "ja357:2.2_0": "Āraññakaṁ pabbatasānugocaraṁ;",
  "ja357:2.3_0": "Pakkhehi taṁ pañjalikaṁ karomi,",
  "ja357:2.4_0": "Mā me vadhī puttake dubbalāya”.",
  "ja357:3.1_0": "“Vadhissāmi te laṭukike puttakāni,",
  "ja357:3.2_0": "Kiṁ me tuvaṁ kāhasi dubbalāsi;",
  "ja357:3.3_0": "Sataṁ sahassānipi tādisīnaṁ,",
  "ja357:3.4_0": "Vāmena pādena papothayeyyaṁ”.",
  "ja357:4.1_0": "“Na heva sabbattha balena kiccaṁ,",
  "ja357:4.2_0": "Balañhi bālassa vadhāya hoti;",
  "ja357:4.3_0": "Karissāmi te nāgarājā anatthaṁ,",
  "ja357:4.4_0": "Yo me vadhī puttake dubbalāya”.",
  "ja357:5.1_0": "“Kākañca passa laṭukikaṁ,",
  "ja357:5.2_0": "Maṇḍūkaṁ nīlamakkhikaṁ;",
  "ja357:5.3_0": "Ete nāgaṁ aghātesuṁ,",
  "ja357:5.4_0": "Passa verassa verinaṁ;",
  "ja357:5.5_0": "Tasmā hi veraṁ na kayirātha,",
  "ja357:5.6_0": "Appiyenapi kenacī”ti.",
  "ja357:6.1_0": "Laṭukikajātakaṁ sattamaṁ."
}