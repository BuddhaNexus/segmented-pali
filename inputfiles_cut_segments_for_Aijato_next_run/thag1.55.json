{
  "thag1.55:0.1_0": "Theragāthā",
  "thag1.55:0.2_0": "Ekakanipāta",
  "thag1.55:0.3_0": "Chaṭṭhavagga",
  "thag1.55:0.4_0": "1.55. Añjanavaniyattheragāthā",
  "thag1.55:1.1_0": "“Āsandiṁ kuṭikaṁ katvā,",
  "thag1.55:1.2_0": "ogayha añjanaṁ vanaṁ;",
  "thag1.55:1.3_0": "Tisso vijjā anuppattā,",
  "thag1.55:1.4_0": "kataṁ buddhassa sāsanan”ti.",
  "thag1.55:1.5_0": "… Añjanavaniyo thero …."
}