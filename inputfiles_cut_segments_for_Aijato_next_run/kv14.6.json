{
  "kv14.6:0.1_0": "Kathāvatthu",
  "kv14.6:0.2_0": "Mahāpaṇṇāsaka",
  "kv14.6:0.3_0": "Cuddasamavagga",
  "kv14.6:0.4_0": "Pariyuṭṭhānaṁcittavippayuttantikathā",
  "kv14.6:1.1_0": "Pariyuṭṭhānaṁ cittavippayuttanti?",
  "kv14.6:1.2_0": "Āmantā.",
  "kv14.6:1.3_0": "Rūpaṁ nibbānaṁ cakkhāyatanaṁ …pe…",
  "kv14.6:1.4_0": "phoṭṭhabbāyatananti?",
  "kv14.6:1.5_0": "Na hevaṁ vattabbe …pe…",
  "kv14.6:1.6_0": "pariyuṭṭhānaṁ cittavippayuttanti?",
  "kv14.6:1.7_0": "Āmantā.",
  "kv14.6:1.8_0": "Natthi sarāgaṁ cittaṁ sadosaṁ cittaṁ samohaṁ cittaṁ …pe…",
  "kv14.6:1.9_0": "akusalaṁ cittaṁ saṅkiliṭṭhaṁ cittanti?",
  "kv14.6:1.10_0": "Na hevaṁ vattabbe …pe…",
  "kv14.6:1.11_0": "nanu atthi sarāgaṁ cittaṁ sadosaṁ cittaṁ samohaṁ cittaṁ …pe…",
  "kv14.6:1.12_0": "akusalaṁ cittaṁ saṅkiliṭṭhaṁ cittanti?",
  "kv14.6:1.13_0": "Āmantā.",
  "kv14.6:1.14_0": "Hañci atthi sarāgaṁ cittaṁ sadosaṁ cittaṁ samohaṁ cittaṁ …pe…",
  "kv14.6:1.15_0": "akusalaṁ cittaṁ saṅkiliṭṭhaṁ cittaṁ, no ca vata re vattabbe—",
  "kv14.6:1.16_0": "“pariyuṭṭhānaṁ cittavippayuttan”ti.",
  "kv14.6:1.17_0": "Pariyuṭṭhānaṁ cittavippayuttantikathā niṭṭhitā."
}