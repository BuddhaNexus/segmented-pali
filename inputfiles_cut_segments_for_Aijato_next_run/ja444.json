{
  "ja444:0.1_0": "Jātaka",
  "ja444:0.2_0": "Dasakanipāta",
  "ja444:0.3_0": "Catudvāravagga",
  "ja444:0.4_0": "6. Kaṇhadīpāyanajātaka",
  "ja444:1.1_0": "“Sattāhamevāhaṁ pasannacitto,",
  "ja444:1.2_0": "Puññatthiko ācariṁ brahmacariyaṁ;",
  "ja444:1.3_0": "Athāparaṁ yaṁ caritaṁ mamedaṁ,",
  "ja444:1.4_0": "Vassāni paññāsa samādhikāni;",
  "ja444:1.5_0": "Akāmakovāpi ahaṁ carāmi,",
  "ja444:1.6_0": "Etena saccena suvatthi hotu;",
  "ja444:1.7_0": "Hataṁ visaṁ jīvatu yaññadatto”.",
  "ja444:2.1_0": "“Yasmā dānaṁ nābhinandiṁ kadāci,",
  "ja444:2.2_0": "Disvānahaṁ atithiṁ vāsakāle;",
  "ja444:2.3_0": "Na cāpi me appiyataṁ aveduṁ,",
  "ja444:2.4_0": "Bahussutā samaṇabrāhmaṇā ca”.",
  "ja444:2.5_0": "Akāmakovāpi ahaṁ dadāmi,",
  "ja444:2.6_0": "Etena saccena suvatthi hotu;",
  "ja444:2.7_0": "Hataṁ visaṁ jīvatu yaññadatto.",
  "ja444:3.1_0": "“Āsīviso tāta pahūtatejo,",
  "ja444:3.2_0": "Yo taṁ adaṁsī sacarā udicca;",
  "ja444:3.3_0": "Tasmiñca me appiyatāya ajja,",
  "ja444:3.4_0": "Pitari ca te natthi koci viseso;",
  "ja444:3.5_0": "Etena saccena suvatthi hotu,",
  "ja444:3.6_0": "Hataṁ visaṁ jīvatu yaññadatto”.",
  "ja444:4.1_0": "“Santā dantāyeva paribbajanti,",
  "ja444:4.2_0": "Aññatra kaṇhā natthākāmarūpā;",
  "ja444:4.3_0": "Dīpāyana kissa jigucchamāno,",
  "ja444:4.4_0": "Akāmako carasi brahmacariyaṁ”.",
  "ja444:5.1_0": "“Saddhāya nikkhamma punaṁ nivatto,",
  "ja444:5.2_0": "So eḷamūgova bālo vatāyaṁ;",
  "ja444:5.3_0": "Etassa vādassa jigucchamāno,",
  "ja444:5.4_0": "Akāmako carāmi brahmacariyaṁ;",
  "ja444:5.5_0": "Viññuppasatthañca satañca ṭhānaṁ,",
  "ja444:5.6_0": "Evampahaṁ puññakaro bhavāmi”.",
  "ja444:6.1_0": "“Samaṇe tuvaṁ brāhmaṇe addhike ca,",
  "ja444:6.2_0": "Santappayāsi annapānena bhikkhaṁ;",
  "ja444:6.3_0": "Opānabhūtaṁva gharaṁ tava yidaṁ,",
  "ja444:6.4_0": "Annena pānena upetarūpaṁ;",
  "ja444:6.5_0": "Atha kissa vādassa jigucchamāno,",
  "ja444:6.6_0": "Akāmako dānamimaṁ dadāsi”.",
  "ja444:7.1_0": "“Pitaro ca me āsuṁ pitāmahā ca,",
  "ja444:7.2_0": "Saddhā ahuṁ dānapatī vadaññū;",
  "ja444:7.3_0": "Taṁ kullavattaṁ anuvattamāno,",
  "ja444:7.4_0": "Māhaṁ kule antimagandhano ahuṁ;",
  "ja444:7.5_0": "Etassa vādassa jigucchamāno,",
  "ja444:7.6_0": "Akāmako dānamimaṁ dadāmi”.",
  "ja444:8.1_0": "“Dahariṁ kumāriṁ asamatthapaññaṁ,",
  "ja444:8.2_0": "Yaṁ tānayiṁ ñātikulā sugatte;",
  "ja444:8.3_0": "Na cāpi me appiyataṁ avedi,",
  "ja444:8.4_0": "Aññatra kāmā paricārayantā;",
  "ja444:8.5_0": "Atha kena vaṇṇena mayā te bhoti,",
  "ja444:8.6_0": "Saṁvāsadhammo ahu evarūpo”.",
  "ja444:9.1_0": "“Ārā dūre nayidha kadāci atthi,",
  "ja444:9.2_0": "Paramparā nāma kule imasmiṁ;",
  "ja444:9.3_0": "Taṁ kullavattaṁ anuvattamānā,",
  "ja444:9.4_0": "Māhaṁ kule antimagandhinī ahuṁ;",
  "ja444:9.5_0": "Etassa vādassa jigucchamānā,",
  "ja444:9.6_0": "Akāmikā paddhacarāmhi tuyhaṁ”.",
  "ja444:10.1_0": "“Maṇḍabya bhāsiṁ yamabhāsaneyyaṁ,",
  "ja444:10.2_0": "Taṁ khamyataṁ puttakahetu majja;",
  "ja444:10.3_0": "Puttapemā na idha paratthi kiñci,",
  "ja444:10.4_0": "So no ayaṁ jīvati yaññadatto”ti.",
  "ja444:11.1_0": "Kaṇhadīpāyanajātakaṁ chaṭṭhaṁ."
}