{
  "tha-ap469:0.1_0": "Therāpadāna",
  "tha-ap469:0.2_0": "Sālakusumiyavagga",
  "tha-ap469:0.3_0": "7 Labujaphaladāyakattheraapadāna",
  "tha-ap469:1.1_0": "“Nagare bandhumatiyā,",
  "tha-ap469:1.2_0": "ārāmiko ahaṁ tadā;",
  "tha-ap469:1.3_0": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap469:1.4_0": "gacchantaṁ anilañjase.",
  "tha-ap469:2.1_0": "Labujaṁ phalamādāya,",
  "tha-ap469:2.2_0": "buddhaseṭṭhassadāsahaṁ;",
  "tha-ap469:2.3_0": "Ākāseva ṭhito santo,",
  "tha-ap469:2.4_0": "paṭiggaṇhi mahāyaso.",
  "tha-ap469:3.1_0": "Vittisañjanano mayhaṁ,",
  "tha-ap469:3.2_0": "diṭṭhadhammasukhāvaho;",
  "tha-ap469:3.3_0": "Phalaṁ buddhassa datvāna,",
  "tha-ap469:3.4_0": "vippasannena cetasā.",
  "tha-ap469:4.1_0": "Adhigañchiṁ tadā pītiṁ,",
  "tha-ap469:4.2_0": "vipulaṁ sukhamuttamaṁ;",
  "tha-ap469:4.3_0": "Uppajjateva ratanaṁ,",
  "tha-ap469:4.4_0": "nibbattassa tahiṁ tahiṁ.",
  "tha-ap469:5.1_0": "Ekanavutito kappe,",
  "tha-ap469:5.2_0": "yaṁ phalaṁ adadiṁ tadā;",
  "tha-ap469:5.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap469:5.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap469:6.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap469:6.2_0": "…pe…",
  "tha-ap469:6.3_0": "viharāmi anāsavo.",
  "tha-ap469:7.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap469:7.2_0": "…pe…",
  "tha-ap469:7.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap469:8.1_0": "Paṭisambhidā catasso,",
  "tha-ap469:8.2_0": "…pe…",
  "tha-ap469:8.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap469:9.1_0": "Itthaṁ sudaṁ āyasmā labujaphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap469:10.1_0": "Labujaphaladāyakattherassāpadānaṁ sattamaṁ."
}