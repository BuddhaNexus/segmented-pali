{
  "tha-ap7:0.1_0": "Therāpadāna",
  "tha-ap7:0.2_0": "Buddhavagga",
  "tha-ap7:0.3_0": "7 Puṇṇamantāṇiputtattheraapadāna",
  "tha-ap7:1.1_0": "“Ajjhāyako mantadharo,",
  "tha-ap7:1.2_0": "tiṇṇaṁ vedāna pāragū;",
  "tha-ap7:1.3_0": "Purakkhatomhi sissehi,",
  "tha-ap7:1.4_0": "upagacchiṁ naruttamaṁ.",
  "tha-ap7:2.1_0": "Padumuttaro lokavidū,",
  "tha-ap7:2.2_0": "āhutīnaṁ paṭiggaho;",
  "tha-ap7:2.3_0": "Mama kammaṁ pakittesi,",
  "tha-ap7:2.4_0": "saṅkhittena mahāmuni.",
  "tha-ap7:3.1_0": "Tāhaṁ dhammaṁ suṇitvāna,",
  "tha-ap7:3.2_0": "Abhivādetvāna satthuno;",
  "tha-ap7:3.3_0": "Añjaliṁ paggahetvāna,",
  "tha-ap7:3.4_0": "Pakkamiṁ dakkhiṇāmukho.",
  "tha-ap7:4.1_0": "Saṅkhittena suṇitvāna,",
  "tha-ap7:4.2_0": "vitthārena abhāsayiṁ;",
  "tha-ap7:4.3_0": "Sabbe sissā attamanā,",
  "tha-ap7:4.4_0": "sutvāna mama bhāsato;",
  "tha-ap7:4.5_0": "Sakaṁ diṭṭhiṁ vinodetvā,",
  "tha-ap7:4.6_0": "buddhe cittaṁ pasādayuṁ.",
  "tha-ap7:5.1_0": "Saṅkhittenapi desemi,",
  "tha-ap7:5.2_0": "vitthārena tathevahaṁ;",
  "tha-ap7:5.3_0": "Abhidhammanayaññūhaṁ,",
  "tha-ap7:5.4_0": "kathāvatthuvisuddhiyā;",
  "tha-ap7:5.5_0": "Sabbesaṁ viññāpetvāna,",
  "tha-ap7:5.6_0": "viharāmi anāsavo.",
  "tha-ap7:6.1_0": "Ito pañcasate kappe,",
  "tha-ap7:6.2_0": "caturo suppakāsakā;",
  "tha-ap7:6.3_0": "Sattaratanasampannā,",
  "tha-ap7:6.4_0": "catudīpamhi issarā.",
  "tha-ap7:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap7:7.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap7:7.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap7:7.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap7:8.1_0": "Itthaṁ sudaṁ āyasmā puṇṇo mantāṇiputto thero imā gāthāyo abhāsitthāti.",
  "tha-ap7:9.1_0": "Puṇṇamantāṇiputtattherassāpadānaṁ pañcamaṁ."
}