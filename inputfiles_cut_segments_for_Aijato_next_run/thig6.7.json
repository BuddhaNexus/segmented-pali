{
  "thig6.7:0.1_0": "Therīgāthā",
  "thig6.7:0.2_0": "Chakkanipāta",
  "thig6.7:0.3_0": "7. Guttātherīgāthā",
  "thig6.7:1.1_0": "“Gutte yadatthaṁ pabbajjā,",
  "thig6.7:1.2_0": "hitvā puttaṁ vasuṁ piyaṁ;",
  "thig6.7:1.3_0": "Tameva anubrūhehi,",
  "thig6.7:1.4_0": "mā cittassa vasaṁ gami.",
  "thig6.7:2.1_0": "Cittena vañcitā sattā,",
  "thig6.7:2.2_0": "mārassa visaye ratā;",
  "thig6.7:2.3_0": "Anekajātisaṁsāraṁ,",
  "thig6.7:2.4_0": "sandhāvanti aviddasū.",
  "thig6.7:3.1_0": "Kāmacchandañca byāpādaṁ,",
  "thig6.7:3.2_0": "sakkāyadiṭṭhimeva ca;",
  "thig6.7:3.3_0": "Sīlabbataparāmāsaṁ,",
  "thig6.7:3.4_0": "vicikicchañca pañcamaṁ.",
  "thig6.7:4.1_0": "Saṁyojanāni etāni,",
  "thig6.7:4.2_0": "pajahitvāna bhikkhunī;",
  "thig6.7:4.3_0": "Orambhāgamanīyāni,",
  "thig6.7:4.4_0": "nayidaṁ punarehisi.",
  "thig6.7:5.1_0": "Rāgaṁ mānaṁ avijjañca,",
  "thig6.7:5.2_0": "uddhaccañca vivajjiya;",
  "thig6.7:5.3_0": "Saṁyojanāni chetvāna,",
  "thig6.7:5.4_0": "dukkhassantaṁ karissasi.",
  "thig6.7:6.1_0": "Khepetvā jātisaṁsāraṁ,",
  "thig6.7:6.2_0": "pariññāya punabbhavaṁ;",
  "thig6.7:6.3_0": "Diṭṭheva dhamme nicchātā,",
  "thig6.7:6.4_0": "upasantā carissatī”ti.",
  "thig6.7:7.1_0": "… Guttā therī …."
}