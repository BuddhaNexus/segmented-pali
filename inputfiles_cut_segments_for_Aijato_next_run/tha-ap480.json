{
  "tha-ap480:0.1_0": "Therāpadāna",
  "tha-ap480:0.2_0": "Naḷamālivagga",
  "tha-ap480:0.3_0": "8. Vallikāraphaladāyakattheraapadāna",
  "tha-ap480:1.1_0": "“Sumano nāma sambuddho,",
  "tha-ap480:1.2_0": "takkarāyaṁ vasī tadā;",
  "tha-ap480:1.3_0": "Vallikāraphalaṁ gayha,",
  "tha-ap480:1.4_0": "sayambhussa adāsahaṁ.",
  "tha-ap480:2.1_0": "Ekatiṁse ito kappe,",
  "tha-ap480:2.2_0": "yaṁ phalaṁ adadiṁ tadā;",
  "tha-ap480:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap480:2.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap480:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap480:3.2_0": "…pe…",
  "tha-ap480:3.3_0": "viharāmi anāsavo.",
  "tha-ap480:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap480:4.2_0": "…pe…",
  "tha-ap480:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap480:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap480:5.2_0": "…pe…",
  "tha-ap480:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap480:6.1_0": "Itthaṁ sudaṁ āyasmā vallikāraphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap480:7.1_0": "Vallikāraphaladāyakattherassāpadānaṁ aṭṭhamaṁ."
}