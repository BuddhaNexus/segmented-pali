{
  "kv15.8:0.1_0": "Kathāvatthu",
  "kv15.8:0.2_0": "Mahāpaṇṇāsaka",
  "kv15.8:0.3_0": "Pannarasamavagga",
  "kv15.8:0.4_0": "Dutiyasaññāvedayitakathā",
  "kv15.8:1.1_0": "Saññāvedayitanirodhasamāpatti lokiyāti?",
  "kv15.8:1.2_0": "Āmantā.",
  "kv15.8:1.3_0": "Rūpanti?",
  "kv15.8:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv15.8:1.5_0": "vedanā …",
  "kv15.8:1.6_0": "saññā …",
  "kv15.8:1.7_0": "saṅkhārā …",
  "kv15.8:1.8_0": "viññāṇanti?",
  "kv15.8:1.9_0": "Na hevaṁ vattabbe …pe…",
  "kv15.8:1.10_0": "kāmāvacarāti?",
  "kv15.8:1.11_0": "Na hevaṁ vattabbe …pe…",
  "kv15.8:1.12_0": "rūpāvacarāti?",
  "kv15.8:1.13_0": "Na hevaṁ vattabbe …pe…",
  "kv15.8:1.14_0": "arūpāvacarāti?",
  "kv15.8:1.15_0": "Na hevaṁ vattabbe …pe….",
  "kv15.8:2.1_0": "Na vattabbaṁ—",
  "kv15.8:2.2_0": "“saññāvedayitanirodhasamāpatti lokiyā”ti?",
  "kv15.8:2.3_0": "Āmantā.",
  "kv15.8:2.4_0": "Lokuttarāti?",
  "kv15.8:2.5_0": "Na hevaṁ vattabbe.",
  "kv15.8:2.6_0": "Tena hi lokiyāti.",
  "kv15.8:2.7_0": "Dutiyasaññāvedayitakathā niṭṭhitā."
}