{
  "cp32:0.1_0": "Cariyāpiṭaka",
  "cp32:0.2_0": "Yudhañjayavagga",
  "cp32:0.3_0": "Saccapāramī 6",
  "cp32:0.4_0": "Sutasomacariya",
  "cp32:1.1_0": "“Punāparaṁ yadā homi,",
  "cp32:1.2_0": "sutasomo mahīpati;",
  "cp32:1.3_0": "Gahito porisādena,",
  "cp32:1.4_0": "brāhmaṇe saṅgaraṁ sariṁ.",
  "cp32:2.1_0": "Khattiyānaṁ ekasataṁ,",
  "cp32:2.2_0": "āvuṇitvā karattale;",
  "cp32:2.3_0": "Etesaṁ pamilāpetvā,",
  "cp32:2.4_0": "yaññatthe upanayī mamaṁ.",
  "cp32:3.1_0": "Apucchi maṁ porisādo,",
  "cp32:3.2_0": "‘kiṁ tvaṁ icchasi nissajaṁ;",
  "cp32:3.3_0": "Yathāmati te kāhāmi,",
  "cp32:3.4_0": "yadi me tvaṁ punehisi’.",
  "cp32:4.1_0": "Tassa paṭissuṇitvāna,",
  "cp32:4.2_0": "paṇhe āgamanaṁ mama;",
  "cp32:4.3_0": "Upagantvā puraṁ rammaṁ,",
  "cp32:4.4_0": "rajjaṁ niyyādayiṁ tadā.",
  "cp32:5.1_0": "Anussaritvā sataṁ dhammaṁ,",
  "cp32:5.2_0": "pubbakaṁ jinasevitaṁ;",
  "cp32:5.3_0": "Brāhmaṇassa dhanaṁ datvā,",
  "cp32:5.4_0": "porisādaṁ upāgamiṁ.",
  "cp32:6.1_0": "Natthi me saṁsayo tattha,",
  "cp32:6.2_0": "Ghātayissati vā na vā;",
  "cp32:6.3_0": "Saccavācānurakkhanto,",
  "cp32:6.4_0": "Jīvitaṁ cajitumupāgamiṁ;",
  "cp32:6.5_0": "Saccena me samo natthi,",
  "cp32:6.6_0": "Esā me saccapāramī”ti.",
  "cp32:7.1_0": "Sutasomacariyaṁ dvādasamaṁ."
}