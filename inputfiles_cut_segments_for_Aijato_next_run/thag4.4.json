{
  "thag4.4:0.1_0": "Theragāthā",
  "thag4.4:0.2_0": "Catukkanipāta",
  "thag4.4:0.3_0": "Paṭhamavagga",
  "thag4.4:0.4_0": "4.4. Nandakattheragāthā",
  "thag4.4:1.1_0": "“Dhiratthu pūre duggandhe,",
  "thag4.4:1.2_0": "mārapakkhe avassute;",
  "thag4.4:1.3_0": "Navasotāni te kāye,",
  "thag4.4:1.4_0": "yāni sandanti sabbadā.",
  "thag4.4:2.1_0": "Mā purāṇaṁ amaññittho,",
  "thag4.4:2.2_0": "māsādesi tathāgate;",
  "thag4.4:2.3_0": "Saggepi te na rajjanti,",
  "thag4.4:2.4_0": "kimaṅgaṁ pana mānuse.",
  "thag4.4:3.1_0": "Ye ca kho bālā dummedhā,",
  "thag4.4:3.2_0": "dummantī mohapārutā;",
  "thag4.4:3.3_0": "Tādisā tattha rajjanti,",
  "thag4.4:3.4_0": "mārakhittamhi bandhane.",
  "thag4.4:4.1_0": "Yesaṁ rāgo ca doso ca,",
  "thag4.4:4.2_0": "avijjā ca virājitā;",
  "thag4.4:4.3_0": "Tādī tattha na rajjanti,",
  "thag4.4:4.4_0": "chinnasuttā abandhanā”ti.",
  "thag4.4:4.5_0": "… Nandako thero …."
}