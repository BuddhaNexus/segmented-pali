{
  "tha-ap323:0.1_0": "Therāpadāna",
  "tha-ap323:0.2_0": "Umāpupphiyavagga",
  "tha-ap323:0.3_0": "1 Umāpupphiyattheraapadāna",
  "tha-ap323:1.1_0": "“Samāhitaṁ samāpannaṁ,",
  "tha-ap323:1.2_0": "siddhatthamaparājitaṁ;",
  "tha-ap323:1.3_0": "Samādhinā upaviṭṭhaṁ,",
  "tha-ap323:1.4_0": "addasāhaṁ naruttamaṁ.",
  "tha-ap323:2.1_0": "Umāpupphaṁ gahetvāna,",
  "tha-ap323:2.2_0": "buddhassa abhiropayiṁ;",
  "tha-ap323:2.3_0": "Sabbapupphā ekasīsā,",
  "tha-ap323:2.4_0": "uddhaṁvaṇṭā adhomukhā.",
  "tha-ap323:3.1_0": "Sucittā viya tiṭṭhante,",
  "tha-ap323:3.2_0": "ākāse pupphasantharā;",
  "tha-ap323:3.3_0": "Tena cittappasādena,",
  "tha-ap323:3.4_0": "tusitaṁ upapajjahaṁ.",
  "tha-ap323:4.1_0": "Catunnavutito kappe,",
  "tha-ap323:4.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap323:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap323:4.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap323:5.1_0": "Pañcapaññāsito kappe,",
  "tha-ap323:5.2_0": "eko āsiṁ mahīpati;",
  "tha-ap323:5.3_0": "Samantachadano nāma,",
  "tha-ap323:5.4_0": "cakkavattī mahabbalo.",
  "tha-ap323:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap323:6.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap323:6.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap323:6.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap323:7.1_0": "Itthaṁ sudaṁ āyasmā umāpupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap323:8.1_0": "Umāpupphiyattherassāpadānaṁ paṭhamaṁ."
}