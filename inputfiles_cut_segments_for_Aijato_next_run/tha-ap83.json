{
  "tha-ap83:0.1_0": "Therāpadāna",
  "tha-ap83:0.2_0": "Timiravagga",
  "tha-ap83:0.3_0": "1. Timirapupphiyattheraapadāna",
  "tha-ap83:1.1_0": "“Candabhāgānadītīre,",
  "tha-ap83:1.2_0": "anusotaṁ vajāmahaṁ;",
  "tha-ap83:1.3_0": "Nisinnaṁ samaṇaṁ disvā,",
  "tha-ap83:1.4_0": "vippasannamanāvilaṁ.",
  "tha-ap83:2.1_0": "Tattha cittaṁ pasādetvā,",
  "tha-ap83:2.2_0": "evaṁ cintesahaṁ tadā;",
  "tha-ap83:2.3_0": "Tārayissati tiṇṇoyaṁ,",
  "tha-ap83:2.4_0": "dantoyaṁ damayissati.",
  "tha-ap83:3.1_0": "Assāsissati assattho,",
  "tha-ap83:3.2_0": "santo ca samayissati;",
  "tha-ap83:3.3_0": "Mocayissati mutto ca,",
  "tha-ap83:3.4_0": "nibbāpessati nibbuto.",
  "tha-ap83:4.1_0": "Evāhaṁ cintayitvāna,",
  "tha-ap83:4.2_0": "siddhatthassa mahesino;",
  "tha-ap83:4.3_0": "Gahetvā timirapupphaṁ,",
  "tha-ap83:4.4_0": "matthake okiriṁ ahaṁ.",
  "tha-ap83:5.1_0": "Añjaliṁ paggahetvāna,",
  "tha-ap83:5.2_0": "katvā ca naṁ padakkhiṇaṁ;",
  "tha-ap83:5.3_0": "Vanditvā satthuno pāde,",
  "tha-ap83:5.4_0": "pakkāmiṁ aparaṁ disaṁ.",
  "tha-ap83:6.1_0": "Aciraṁ gatamattaṁ maṁ,",
  "tha-ap83:6.2_0": "migarājā viheṭhayi;",
  "tha-ap83:6.3_0": "Papātamanugacchanto,",
  "tha-ap83:6.4_0": "tattheva papatiṁ ahaṁ.",
  "tha-ap83:7.1_0": "Catunnavutito kappe,",
  "tha-ap83:7.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap83:7.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap83:7.4_0": "pupphapūjāyidaṁ phalaṁ.",
  "tha-ap83:8.1_0": "Chappaññāsamhi kappamhi,",
  "tha-ap83:8.2_0": "sattevāsuṁ mahāyasā;",
  "tha-ap83:8.3_0": "Sattaratanasampannā,",
  "tha-ap83:8.4_0": "cakkavattī mahabbalā.",
  "tha-ap83:9.1_0": "Paṭisambhidā catasso,",
  "tha-ap83:9.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap83:9.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap83:9.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap83:10.1_0": "Itthaṁ sudaṁ āyasmā timirapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap83:11.1_0": "Timirapupphiyattherassāpadānaṁ paṭhamaṁ."
}