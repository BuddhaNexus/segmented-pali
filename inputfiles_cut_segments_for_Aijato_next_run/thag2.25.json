{
  "thag2.25:0.1_0": "Theragāthā",
  "thag2.25:0.2_0": "Dukanipāta",
  "thag2.25:0.3_0": "Tatiyavagga",
  "thag2.25:0.4_0": "2.25. Vītasokattheragāthā",
  "thag2.25:1.1_0": "“Kese me olikhissanti,",
  "thag2.25:1.2_0": "kappako upasaṅkami;",
  "thag2.25:1.3_0": "Tato ādāsamādāya,",
  "thag2.25:1.4_0": "sarīraṁ paccavekkhisaṁ.",
  "thag2.25:2.1_0": "Tuccho kāyo adissittha,",
  "thag2.25:2.2_0": "andhakāro tamo byagā;",
  "thag2.25:2.3_0": "Sabbe coḷā samucchinnā,",
  "thag2.25:2.4_0": "natthi dāni punabbhavo”ti.",
  "thag2.25:2.5_0": "… Vītasoko thero …."
}