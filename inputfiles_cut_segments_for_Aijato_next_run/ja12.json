{
  "ja12:0.1_0": "Jātaka",
  "ja12:0.2_0": "Ekakanipāta",
  "ja12:0.3_0": "Sīlavagga",
  "ja12:0.4_0": "2. Nigrodhamigajātaka",
  "ja12:1.1_0": "“Nigrodhameva seveyya,",
  "ja12:1.2_0": "na sākhamupasaṁvase;",
  "ja12:1.3_0": "Nigrodhasmiṁ mataṁ seyyo,",
  "ja12:1.4_0": "yañce sākhasmi jīvitan”ti.",
  "ja12:2.1_0": "Nigrodhamigajātakaṁ dutiyaṁ."
}