{
  "tha-ap210:0.1_0": "Therāpadāna",
  "tha-ap210:0.2_0": "Kaṇikārapupphiyavagga",
  "tha-ap210:0.3_0": "8. Koraṇḍapupphiyattheraapadāna",
  "tha-ap210:1.1_0": "“Akkantañca padaṁ disvā,",
  "tha-ap210:1.2_0": "cakkālaṅkārabhūsitaṁ;",
  "tha-ap210:1.3_0": "Padenānupadaṁ yanto,",
  "tha-ap210:1.4_0": "vipassissa mahesino.",
  "tha-ap210:2.1_0": "Koraṇḍaṁ pupphitaṁ disvā,",
  "tha-ap210:2.2_0": "samūlaṁ pūjitaṁ mayā;",
  "tha-ap210:2.3_0": "Haṭṭho haṭṭhena cittena,",
  "tha-ap210:2.4_0": "avandiṁ padamuttamaṁ.",
  "tha-ap210:3.1_0": "Ekanavutito kappe,",
  "tha-ap210:3.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap210:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap210:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap210:4.1_0": "Sattapaññāsakappamhi,",
  "tha-ap210:4.2_0": "eko vītamalo ahuṁ;",
  "tha-ap210:4.3_0": "Sattaratanasampanno,",
  "tha-ap210:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap210:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap210:5.2_0": "…pe…",
  "tha-ap210:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap210:6.1_0": "Itthaṁ sudaṁ āyasmā koraṇḍapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap210:7.1_0": "Koraṇḍapupphiyattherassāpadānaṁ aṭṭhamaṁ."
}