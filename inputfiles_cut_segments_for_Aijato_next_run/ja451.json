{
  "ja451:0.1_0": "Jātaka",
  "ja451:0.2_0": "Dasakanipāta",
  "ja451:0.3_0": "Catudvāravagga",
  "ja451:0.4_0": "13. Cakkavākajātaka",
  "ja451:1.1_0": "“Vaṇṇavā abhirūposi,",
  "ja451:1.2_0": "ghano sañjātarohito;",
  "ja451:1.3_0": "Cakkavāka surūposi,",
  "ja451:1.4_0": "vippasannamukhindriyo.",
  "ja451:2.1_0": "Pāṭhīnaṁ pāvusaṁ macchaṁ,",
  "ja451:2.2_0": "balajaṁ muñjarohitaṁ;",
  "ja451:2.3_0": "Gaṅgāya tīre nisinno,",
  "ja451:2.4_0": "evaṁ bhuñjasi bhojanaṁ”.",
  "ja451:3.1_0": "“Na vāhametaṁ bhuñjāmi,",
  "ja451:3.2_0": "Jaṅgalānodakāni vā;",
  "ja451:3.3_0": "Aññatra sevālapaṇakā,",
  "ja451:3.4_0": "Etaṁ me samma bhojanaṁ”.",
  "ja451:4.1_0": "“Na vāhametaṁ saddahāmi,",
  "ja451:4.2_0": "cakkavākassa bhojanaṁ;",
  "ja451:4.3_0": "Ahampi samma bhuñjāmi,",
  "ja451:4.4_0": "gāme loṇiyateliyaṁ.",
  "ja451:5.1_0": "Manussesu kataṁ bhattaṁ,",
  "ja451:5.2_0": "suciṁ maṁsūpasecanaṁ;",
  "ja451:5.3_0": "Na ca me tādiso vaṇṇo,",
  "ja451:5.4_0": "cakkavāka yathā tuvaṁ”.",
  "ja451:6.1_0": "“Sampassaṁ attani veraṁ,",
  "ja451:6.2_0": "hiṁsayaṁ mānusiṁ pajaṁ;",
  "ja451:6.3_0": "Utrasto ghasasī bhīto,",
  "ja451:6.4_0": "tena vaṇṇo tavediso.",
  "ja451:7.1_0": "Sabbalokaviruddhosi,",
  "ja451:7.2_0": "dhaṅka pāpena kammunā;",
  "ja451:7.3_0": "Laddho piṇḍo na pīṇeti,",
  "ja451:7.4_0": "tena vaṇṇo tavediso.",
  "ja451:8.1_0": "Ahampi samma bhuñjāmi,",
  "ja451:8.2_0": "ahiṁsaṁ sabbapāṇinaṁ;",
  "ja451:8.3_0": "Appossukko nirāsaṅkī,",
  "ja451:8.4_0": "asoko akutobhayo.",
  "ja451:9.1_0": "So karassu ānubhāvaṁ,",
  "ja451:9.2_0": "vītivattassu sīliyaṁ;",
  "ja451:9.3_0": "Ahiṁsāya cara loke,",
  "ja451:9.4_0": "piyo hohisi maṁmiva.",
  "ja451:10.1_0": "Yo na hanti na ghāteti,",
  "ja451:10.2_0": "na jināti na jāpaye;",
  "ja451:10.3_0": "Mettaṁso sabbabhūtesu,",
  "ja451:10.4_0": "veraṁ tassa na kenacī”ti.",
  "ja451:11.1_0": "Cakkavākajātakaṁ terasamaṁ."
}