{
  "ja334:0.1_0": "Jātaka",
  "ja334:0.2_0": "Catukkanipāta",
  "ja334:0.3_0": "Kokilavagga",
  "ja334:0.4_0": "4. Rājovādajātaka",
  "ja334:1.1_0": "“Gavañce taramānānaṁ,",
  "ja334:1.2_0": "jimhaṁ gacchati puṅgavo;",
  "ja334:1.3_0": "Sabbā tā jimhaṁ gacchanti,",
  "ja334:1.4_0": "nette jimhaṁ gate sati.",
  "ja334:2.1_0": "Evameva manussesu,",
  "ja334:2.2_0": "yo hoti seṭṭhasammato;",
  "ja334:2.3_0": "So ce adhammaṁ carati,",
  "ja334:2.4_0": "pageva itarā pajā;",
  "ja334:2.5_0": "Sabbaṁ raṭṭhaṁ dukhaṁ seti,",
  "ja334:2.6_0": "rājā ce hoti adhammiko.",
  "ja334:3.1_0": "Gavañce taramānānaṁ,",
  "ja334:3.2_0": "ujuṁ gacchati puṅgavo;",
  "ja334:3.3_0": "Sabbā gāvī ujuṁ yanti,",
  "ja334:3.4_0": "nette ujuṁ gate sati.",
  "ja334:4.1_0": "Evameva manussesu,",
  "ja334:4.2_0": "yo hoti seṭṭhasammato;",
  "ja334:4.3_0": "So sace dhammaṁ carati,",
  "ja334:4.4_0": "pageva itarā pajā;",
  "ja334:4.5_0": "Sabbaṁ raṭṭhaṁ sukhaṁ seti,",
  "ja334:4.6_0": "rājā ce hoti dhammiko”ti.",
  "ja334:5.1_0": "Rājovādajātakaṁ catutthaṁ."
}