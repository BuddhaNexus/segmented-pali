{
  "tha-ap67:0.1_0": "Therāpadāna",
  "tha-ap67:0.2_0": "Sakacintaniyavagga",
  "tha-ap67:0.3_0": "5. Bhisadāyakattheraapadāna",
  "tha-ap67:1.1_0": "“Vessabhū nāma nāmena,",
  "tha-ap67:1.2_0": "isīnaṁ tatiyo ahu;",
  "tha-ap67:1.3_0": "Kānanaṁ vanamogayha,",
  "tha-ap67:1.4_0": "vihāsi purisuttamo.",
  "tha-ap67:2.1_0": "Bhisamuḷālaṁ gaṇhitvā,",
  "tha-ap67:2.2_0": "agamaṁ buddhasantikaṁ;",
  "tha-ap67:2.3_0": "Tañca buddhassa pādāsiṁ,",
  "tha-ap67:2.4_0": "pasanno sehi pāṇibhi.",
  "tha-ap67:3.1_0": "Karena ca parāmaṭṭho,",
  "tha-ap67:3.2_0": "vessabhūvarabuddhinā;",
  "tha-ap67:3.3_0": "Sukhāhaṁ nābhijānāmi,",
  "tha-ap67:3.4_0": "samaṁ tena kutottariṁ.",
  "tha-ap67:4.1_0": "Carimo vattate mayhaṁ,",
  "tha-ap67:4.2_0": "bhavā sabbe samūhatā;",
  "tha-ap67:4.3_0": "Hatthināgena santena,",
  "tha-ap67:4.4_0": "kusalaṁ ropitaṁ mayā.",
  "tha-ap67:5.1_0": "Ekattiṁse ito kappe,",
  "tha-ap67:5.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap67:5.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap67:5.4_0": "bhisadānassidaṁ phalaṁ.",
  "tha-ap67:6.1_0": "Samodhānā ca rājāno,",
  "tha-ap67:6.2_0": "soḷasa manujādhipā;",
  "tha-ap67:6.3_0": "Kappamhi cuddase āsuṁ,",
  "tha-ap67:6.4_0": "cakkavattī mahabbalā.",
  "tha-ap67:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap67:7.2_0": "…pe…",
  "tha-ap67:7.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap67:8.1_0": "Itthaṁ sudaṁ āyasmā bhisadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap67:9.1_0": "Bhisadāyakattherassāpadānaṁ pañcamaṁ."
}