{
  "tha-ap290:0.1_0": "Therāpadāna",
  "tha-ap290:0.2_0": "Paṇṇadāyakavagga",
  "tha-ap290:0.3_0": "8. Sattāhapabbajitattheraapadāna",
  "tha-ap290:1.1_0": "“Vipassissa bhagavato,",
  "tha-ap290:1.2_0": "saṅgho sakkatamānito;",
  "tha-ap290:1.3_0": "Byasanaṁ me anuppattaṁ,",
  "tha-ap290:1.4_0": "ñātibhedo pure ahu.",
  "tha-ap290:2.1_0": "Pabbajjaṁ upagantvāna,",
  "tha-ap290:2.2_0": "byasanupasamāyahaṁ;",
  "tha-ap290:2.3_0": "Sattāhābhirato tattha,",
  "tha-ap290:2.4_0": "satthusāsanakamyatā.",
  "tha-ap290:3.1_0": "Ekanavutito kappe,",
  "tha-ap290:3.2_0": "yamahaṁ pabbajiṁ tadā;",
  "tha-ap290:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap290:3.4_0": "pabbajjāya idaṁ phalaṁ.",
  "tha-ap290:4.1_0": "Sattasaṭṭhimhito kappe,",
  "tha-ap290:4.2_0": "satta āsuṁ mahīpatī;",
  "tha-ap290:4.3_0": "Sunikkhamāti ñāyanti,",
  "tha-ap290:4.4_0": "cakkavattī mahabbalā.",
  "tha-ap290:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap290:5.2_0": "…pe…",
  "tha-ap290:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap290:6.1_0": "Itthaṁ sudaṁ āyasmā sattāhapabbajito thero imā gāthāyo abhāsitthāti.",
  "tha-ap290:7.1_0": "Sattāhapabbajitattherassāpadānaṁ aṭṭhamaṁ."
}