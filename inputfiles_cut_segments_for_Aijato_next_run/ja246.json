{
  "ja246:0.1_0": "Jātaka",
  "ja246:0.2_0": "Dukanipāta",
  "ja246:0.3_0": "Siṅgālavagga",
  "ja246:0.4_0": "6. Bālovādajātaka",
  "ja246:1.1_0": "“Hantvā chetvā vadhitvā ca,",
  "ja246:1.2_0": "deti dānaṁ asaññato;",
  "ja246:1.3_0": "Edisaṁ bhattaṁ bhuñjamāno,",
  "ja246:1.4_0": "sa pāpena upalippati”.",
  "ja246:2.1_0": "“Puttadārampi ce hantvā,",
  "ja246:2.2_0": "Deti dānaṁ asaññato;",
  "ja246:2.3_0": "Bhuñjamānopi sappañño,",
  "ja246:2.4_0": "Na pāpena upalippatī”ti.",
  "ja246:3.1_0": "Bālovādajātakaṁ chaṭṭhaṁ."
}