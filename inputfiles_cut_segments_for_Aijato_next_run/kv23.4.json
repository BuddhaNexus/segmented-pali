{
  "kv23.4:0.1_0": "Kathāvatthu",
  "kv23.4:0.2_0": "Mahāpaṇṇāsaka",
  "kv23.4:0.3_0": "Tevīsatimavagga",
  "kv23.4:0.4_0": "Patirūpakathā",
  "kv23.4:1.1_0": "Atthi na rāgo rāgapatirūpakoti?",
  "kv23.4:1.2_0": "Āmantā.",
  "kv23.4:1.3_0": "Atthi na phasso phassapatirūpako,",
  "kv23.4:1.4_0": "atthi na vedanā vedanāpatirūpikā,",
  "kv23.4:1.5_0": "atthi na saññā saññāpatirūpikā,",
  "kv23.4:1.6_0": "atthi na cetanā cetanāpatirūpikā,",
  "kv23.4:1.7_0": "atthi na cittaṁ cittapatirūpakaṁ,",
  "kv23.4:1.8_0": "atthi na saddhā saddhāpatirūpikā,",
  "kv23.4:1.9_0": "atthi na vīriyaṁ vīriyapatirūpakaṁ,",
  "kv23.4:1.10_0": "atthi na sati satipatirūpikā,",
  "kv23.4:1.11_0": "atthi na samādhi samādhipatirūpako,",
  "kv23.4:1.12_0": "atthi na paññā paññāpatirūpikāti?",
  "kv23.4:1.13_0": "Na hevaṁ vattabbe …pe….",
  "kv23.4:2.1_0": "Atthi na doso dosapatirūpako,",
  "kv23.4:2.2_0": "atthi na moho mohapatirūpako,",
  "kv23.4:2.3_0": "atthi na kileso kilesapatirūpakoti?",
  "kv23.4:2.4_0": "Āmantā.",
  "kv23.4:2.5_0": "Atthi na phasso phassapatirūpako …pe…",
  "kv23.4:2.6_0": "atthi na paññā paññāpatirūpikāti?",
  "kv23.4:2.7_0": "Na hevaṁ vattabbe …pe….",
  "kv23.4:2.8_0": "Patirūpakathā niṭṭhitā."
}