{
  "tha-ap152:0.1_0": "Therāpadāna",
  "tha-ap152:0.2_0": "Chattavagga",
  "tha-ap152:0.3_0": "10. Pallaṅkadāyakattheraapadāna",
  "tha-ap152:1.1_0": "“Sumedhassa bhagavato,",
  "tha-ap152:1.2_0": "lokajeṭṭhassa tādino;",
  "tha-ap152:1.3_0": "Pallaṅko hi mayā dinno,",
  "tha-ap152:1.4_0": "sauttarasapacchado.",
  "tha-ap152:2.1_0": "Sattaratanasampanno,",
  "tha-ap152:2.2_0": "pallaṅko āsi so tadā;",
  "tha-ap152:2.3_0": "Mama saṅkappamaññāya,",
  "tha-ap152:2.4_0": "nibbattati sadā mama.",
  "tha-ap152:3.1_0": "Tiṁsakappasahassamhi,",
  "tha-ap152:3.2_0": "pallaṅkamadadiṁ tadā;",
  "tha-ap152:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap152:3.4_0": "pallaṅkassa idaṁ phalaṁ.",
  "tha-ap152:4.1_0": "Vīsakappasahassamhi,",
  "tha-ap152:4.2_0": "suvaṇṇābhā tayo janā;",
  "tha-ap152:4.3_0": "Sattaratanasampannā,",
  "tha-ap152:4.4_0": "cakkavattī mahabbalā.",
  "tha-ap152:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap152:5.2_0": "…pe…",
  "tha-ap152:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap152:6.1_0": "Itthaṁ sudaṁ āyasmā pallaṅkadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap152:7.1_0": "Pallaṅkadāyakattherassāpadānaṁ dasamaṁ.",
  "tha-ap152:8.1_0": "Chattavaggo pannarasamo.",
  "tha-ap152:9.1_0": "Tassuddānaṁ",
  "tha-ap152:10.1_0": "Chattaṁ thambho ca vedi ca,",
  "tha-ap152:10.2_0": "parivārumapupphiyo;",
  "tha-ap152:10.3_0": "Anulepo ca maggo ca,",
  "tha-ap152:10.4_0": "phalako ca vaṭaṁsako;",
  "tha-ap152:10.5_0": "Pallaṅkadāyī ca gāthāyo,",
  "tha-ap152:10.6_0": "chappaññāsa pakittitāti."
}