{
  "ja362:0.1_0": "Jātaka",
  "ja362:0.2_0": "Pañcakanipāta",
  "ja362:0.3_0": "Vaṇṇārohavagga",
  "ja362:0.4_0": "2. Sīlavīmaṁsajātaka",
  "ja362:1.1_0": "“Sīlaṁ seyyo sutaṁ seyyo,",
  "ja362:1.2_0": "iti me saṁsayo ahu;",
  "ja362:1.3_0": "Sīlameva sutā seyyo,",
  "ja362:1.4_0": "iti me natthi saṁsayo.",
  "ja362:2.1_0": "Moghā jāti ca vaṇṇo ca,",
  "ja362:2.2_0": "sīlameva kiruttamaṁ;",
  "ja362:2.3_0": "Sīlena anupetassa,",
  "ja362:2.4_0": "sutenattho na vijjati.",
  "ja362:3.1_0": "Khattiyo ca adhammaṭṭho,",
  "ja362:3.2_0": "vesso cādhammanissito;",
  "ja362:3.3_0": "Te pariccajjubho loke,",
  "ja362:3.4_0": "upapajjanti duggatiṁ.",
  "ja362:4.1_0": "Khattiyā brāhmaṇā vessā,",
  "ja362:4.2_0": "suddā caṇḍālapukkusā;",
  "ja362:4.3_0": "Idha dhammaṁ caritvāna,",
  "ja362:4.4_0": "bhavanti tidive samā.",
  "ja362:5.1_0": "Na vedā samparāyāya,",
  "ja362:5.2_0": "Na jāti nāpi bandhavā;",
  "ja362:5.3_0": "Sakañca sīlaṁ saṁsuddhaṁ,",
  "ja362:5.4_0": "Samparāyāya sukhāya cā”ti.",
  "ja362:6.1_0": "Sīlavīmaṁsajātakaṁ dutiyaṁ."
}