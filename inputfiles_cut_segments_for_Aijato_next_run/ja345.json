{
  "ja345:0.1_0": "Jātaka",
  "ja345:0.2_0": "Catukkanipāta",
  "ja345:0.3_0": "Cūḷakuṇālavagga",
  "ja345:0.4_0": "5. Gajakumbhajātaka",
  "ja345:1.1_0": "“Vanaṁ yadaggi dahati,",
  "ja345:1.2_0": "pāvako kaṇhavattanī;",
  "ja345:1.3_0": "Kathaṁ karosi pacalaka,",
  "ja345:1.4_0": "evaṁ dandhaparakkamo”.",
  "ja345:2.1_0": "“Bahūni rukkhachiddāni,",
  "ja345:2.2_0": "pathabyā vivarāni ca;",
  "ja345:2.3_0": "Tāni ce nābhisambhoma,",
  "ja345:2.4_0": "hoti no kālapariyāyo”.",
  "ja345:3.1_0": "“Yo dandhakāle tarati,",
  "ja345:3.2_0": "taraṇīye ca dandhati;",
  "ja345:3.3_0": "Sukkhapaṇṇaṁva akkamma,",
  "ja345:3.4_0": "atthaṁ bhañjati attano.",
  "ja345:4.1_0": "Yo dandhakāle dandheti,",
  "ja345:4.2_0": "taraṇīye ca tārayi;",
  "ja345:4.3_0": "Sasīva rattiṁ vibhajaṁ,",
  "ja345:4.4_0": "tassattho paripūratī”ti.",
  "ja345:5.1_0": "Gajakumbhajātakaṁ pañcamaṁ."
}