{
  "cp12:0.1_0": "Cariyāpiṭaka",
  "cp12:0.2_0": "Hatthināgavagga",
  "cp12:0.3_0": "Sīlapāramī 2",
  "cp12:0.4_0": "Bhūridattacariya",
  "cp12:1.1_0": "“Punāparaṁ yadā homi,",
  "cp12:1.2_0": "bhūridatto mahiddhiko;",
  "cp12:1.3_0": "Virūpakkhena mahāraññā,",
  "cp12:1.4_0": "devalokamagañchahaṁ.",
  "cp12:2.1_0": "Tattha passitvāhaṁ deve,",
  "cp12:2.2_0": "ekantaṁ sukhasamappite;",
  "cp12:2.3_0": "Taṁ saggagamanatthāya,",
  "cp12:2.4_0": "sīlabbataṁ samādiyiṁ.",
  "cp12:3.1_0": "Sarīrakiccaṁ katvāna,",
  "cp12:3.2_0": "bhutvā yāpanamattakaṁ;",
  "cp12:3.3_0": "Caturo aṅge adhiṭṭhāya,",
  "cp12:3.4_0": "semi vammikamuddhani.",
  "cp12:4.1_0": "‘Chaviyā cammena maṁsena,",
  "cp12:4.2_0": "Nahāruaṭṭhikehi vā;",
  "cp12:4.3_0": "Yassa etena karaṇīyaṁ,",
  "cp12:4.4_0": "Dinnaṁyeva harātu so’.",
  "cp12:5.1_0": "Saṁsito akataññunā,",
  "cp12:5.2_0": "ālampāyano mamaggahi;",
  "cp12:5.3_0": "Peḷāya pakkhipitvāna,",
  "cp12:5.4_0": "kīḷeti maṁ tahiṁ tahiṁ.",
  "cp12:6.1_0": "Peḷāya pakkhipantepi,",
  "cp12:6.2_0": "sammaddantepi pāṇinā;",
  "cp12:6.3_0": "Ālampāyane na kuppāmi,",
  "cp12:6.4_0": "sīlakhaṇḍabhayā mama.",
  "cp12:7.1_0": "Sakajīvitapariccāgo,",
  "cp12:7.2_0": "tiṇato lahuko mama;",
  "cp12:7.3_0": "Sīlavītikkamo mayhaṁ,",
  "cp12:7.4_0": "pathavīuppatanaṁ viya.",
  "cp12:8.1_0": "Nirantaraṁ jātisataṁ,",
  "cp12:8.2_0": "cajeyyaṁ mama jīvitaṁ;",
  "cp12:8.3_0": "Neva sīlaṁ pabhindeyyaṁ,",
  "cp12:8.4_0": "catuddīpāna hetupi.",
  "cp12:9.1_0": "Api cāhaṁ sīlarakkhāya,",
  "cp12:9.2_0": "Sīlapāramipūriyā;",
  "cp12:9.3_0": "Na karomi citte aññathattaṁ,",
  "cp12:9.4_0": "Pakkhipantampi peḷake”ti.",
  "cp12:10.1_0": "Bhūridattacariyaṁ dutiyaṁ."
}