{
  "tha-ap112:0.1_0": "Therāpadāna",
  "tha-ap112:0.2_0": "Bhikkhadāyivagga",
  "tha-ap112:0.3_0": "10. Reṇupūjakattheraapadāna",
  "tha-ap112:1.1_0": "“Suvaṇṇavaṇṇaṁ sambuddhaṁ,",
  "tha-ap112:1.2_0": "Sataraṁsiṁva bhāṇumaṁ;",
  "tha-ap112:1.3_0": "Obhāsentaṁ disā sabbā,",
  "tha-ap112:1.4_0": "Uḷurājaṁva pūritaṁ.",
  "tha-ap112:2.1_0": "Purakkhataṁ sāvakehi,",
  "tha-ap112:2.2_0": "sāgareheva medaniṁ;",
  "tha-ap112:2.3_0": "Nāgaṁ paggayha reṇūhi,",
  "tha-ap112:2.4_0": "vipassissābhiropayiṁ.",
  "tha-ap112:3.1_0": "Ekanavutito kappe,",
  "tha-ap112:3.2_0": "yaṁ reṇumabhiropayiṁ;",
  "tha-ap112:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap112:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap112:4.1_0": "Paṇṇatālīsito kappe,",
  "tha-ap112:4.2_0": "reṇu nāmāsi khattiyo;",
  "tha-ap112:4.3_0": "Sattaratanasampanno,",
  "tha-ap112:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap112:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap112:5.2_0": "…pe…",
  "tha-ap112:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap112:6.1_0": "Itthaṁ sudaṁ āyasmā reṇupūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap112:7.1_0": "Reṇupūjakattherassāpadānaṁ dasamaṁ.",
  "tha-ap112:8.1_0": "Bhikkhadāyivaggo ekādasamo.",
  "tha-ap112:9.1_0": "Tassuddānaṁ",
  "tha-ap112:10.1_0": "Bhikkhadāyī ñāṇasaññī,",
  "tha-ap112:10.2_0": "hatthiyo padapūjako;",
  "tha-ap112:10.3_0": "Muṭṭhipupphī udakado,",
  "tha-ap112:10.4_0": "naḷamāli upaṭṭhako;",
  "tha-ap112:10.5_0": "Biḷālidāyī reṇu ca,",
  "tha-ap112:10.6_0": "gāthāyo cha ca saṭṭhi ca."
}