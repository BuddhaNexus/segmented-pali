{
  "ja258:0.1_0": "Jātaka",
  "ja258:0.2_0": "Tikanipāta",
  "ja258:0.3_0": "Saṅkappavagga",
  "ja258:0.4_0": "8. Mandhātujātaka",
  "ja258:1.1_0": "“Yāvatā candimasūriyā,",
  "ja258:1.2_0": "Pariharanti disā bhanti virocanā;",
  "ja258:1.3_0": "Sabbeva dāsā mandhātu,",
  "ja258:1.4_0": "Ye pāṇā pathavissitā.",
  "ja258:2.1_0": "Na kahāpaṇavassena,",
  "ja258:2.2_0": "titti kāmesu vijjati;",
  "ja258:2.3_0": "Appassādā dukhā kāmā,",
  "ja258:2.4_0": "iti viññāya paṇḍito.",
  "ja258:3.1_0": "Api dibbesu kāmesu,",
  "ja258:3.2_0": "ratiṁ so nādhigacchati;",
  "ja258:3.3_0": "Taṇhakkhayarato hoti,",
  "ja258:3.4_0": "sammāsambuddhasāvako”ti.",
  "ja258:4.1_0": "Mandhātujātakaṁ aṭṭhamaṁ."
}