{
  "tha-ap136:0.1_0": "Therāpadāna",
  "tha-ap136:0.2_0": "Sobhitavagga",
  "tha-ap136:0.3_0": "4 Pupphacchadaniyattheraapadāna",
  "tha-ap136:1.1_0": "“Sunando nāma nāmena,",
  "tha-ap136:1.2_0": "brāhmaṇo mantapāragū;",
  "tha-ap136:1.3_0": "Ajjhāyako yācayogo,",
  "tha-ap136:1.4_0": "vājapeyyaṁ ayājayi.",
  "tha-ap136:2.1_0": "Padumuttaro lokavidū,",
  "tha-ap136:2.2_0": "aggo kāruṇiko isi;",
  "tha-ap136:2.3_0": "Janataṁ anukampanto,",
  "tha-ap136:2.4_0": "ambare caṅkamī tadā.",
  "tha-ap136:3.1_0": "Caṅkamitvāna sambuddho,",
  "tha-ap136:3.2_0": "sabbaññū lokanāyako;",
  "tha-ap136:3.3_0": "Mettāya aphari satte,",
  "tha-ap136:3.4_0": "appamāṇe nirūpadhi.",
  "tha-ap136:4.1_0": "Vaṇṭe chetvāna pupphāni,",
  "tha-ap136:4.2_0": "brāhmaṇo mantapāragū;",
  "tha-ap136:4.3_0": "Sabbe sisse samānetvā,",
  "tha-ap136:4.4_0": "ākāse ukkhipāpayi.",
  "tha-ap136:5.1_0": "Yāvatā nagaraṁ āsi,",
  "tha-ap136:5.2_0": "pupphānaṁ chadanaṁ tadā;",
  "tha-ap136:5.3_0": "Buddhassa ānubhāvena,",
  "tha-ap136:5.4_0": "sattāhaṁ na vigacchatha.",
  "tha-ap136:6.1_0": "Teneva sukkamūlena,",
  "tha-ap136:6.2_0": "anubhotvāna sampadā;",
  "tha-ap136:6.3_0": "Sabbāsave pariññāya,",
  "tha-ap136:6.4_0": "tiṇṇo loke visattikaṁ.",
  "tha-ap136:7.1_0": "Ekārase kappasate,",
  "tha-ap136:7.2_0": "pañcatiṁsāsu khattiyā;",
  "tha-ap136:7.3_0": "Ambaraṁsasanāmā te,",
  "tha-ap136:7.4_0": "cakkavattī mahabbalā.",
  "tha-ap136:8.1_0": "Paṭisambhidā catasso,",
  "tha-ap136:8.2_0": "…pe…",
  "tha-ap136:8.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap136:9.1_0": "Itthaṁ sudaṁ āyasmā pupphacchadaniyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap136:10.1_0": "Pupphacchadaniyattherassāpadānaṁ catutthaṁ."
}