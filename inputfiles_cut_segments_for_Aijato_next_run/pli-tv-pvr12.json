{
  "pli-tv-pvr12:0.1_0": "Parivāra",
  "pli-tv-pvr12:0.2_0": "Aparagāthāsaṅgaṇika",
  "pli-tv-pvr12:0.3_0": "1. Codanādipucchāvissajjanā",
  "pli-tv-pvr12:1.1_0": "Codanā kimatthāya,",
  "pli-tv-pvr12:1.2_0": "Sāraṇā kissa kāraṇā;",
  "pli-tv-pvr12:1.3_0": "Saṅgho kimatthāya,",
  "pli-tv-pvr12:1.4_0": "Matikammaṁ pana kissa kāraṇā.",
  "pli-tv-pvr12:2.1_0": "Codanā sāraṇatthāya,",
  "pli-tv-pvr12:2.2_0": "Niggahatthāya sāraṇā;",
  "pli-tv-pvr12:2.3_0": "Saṅgho pariggahatthāya,",
  "pli-tv-pvr12:2.4_0": "Matikammaṁ pana pāṭiyekkaṁ.",
  "pli-tv-pvr12:3.1_0": "Mā kho turito abhaṇi,",
  "pli-tv-pvr12:3.2_0": "mā kho caṇḍikato bhaṇi;",
  "pli-tv-pvr12:3.3_0": "Mā kho paṭighaṁ janayi,",
  "pli-tv-pvr12:3.4_0": "sace anuvijjako tuvaṁ.",
  "pli-tv-pvr12:4.1_0": "Mā kho sahasā abhaṇi,",
  "pli-tv-pvr12:4.2_0": "Kathaṁ viggāhikaṁ anatthasaṁhitaṁ;",
  "pli-tv-pvr12:4.3_0": "Sutte vinaye anulome,",
  "pli-tv-pvr12:4.4_0": "Paññatte anulomike.",
  "pli-tv-pvr12:5.1_0": "Anuyogavattaṁ nisāmaya,",
  "pli-tv-pvr12:5.2_0": "Kusalena buddhimatā kataṁ;",
  "pli-tv-pvr12:5.3_0": "Suvuttaṁ sikkhāpadānulomikaṁ,",
  "pli-tv-pvr12:5.4_0": "Gatiṁ na nāsento samparāyikaṁ;",
  "pli-tv-pvr12:5.5_0": "Hitesī anuyuñjassu,",
  "pli-tv-pvr12:5.6_0": "Kālenatthūpasaṁhitaṁ.",
  "pli-tv-pvr12:6.1_0": "Cuditassa ca codakassa ca,",
  "pli-tv-pvr12:6.2_0": "Sahasā vohāraṁ mā padhāresi;",
  "pli-tv-pvr12:6.3_0": "Codako āha āpannoti,",
  "pli-tv-pvr12:6.4_0": "Cuditako āha anāpannoti.",
  "pli-tv-pvr12:7.1_0": "Ubho anukkhipanto,",
  "pli-tv-pvr12:7.2_0": "Paṭiññānusandhitena kāraye;",
  "pli-tv-pvr12:7.3_0": "Paṭiññā lajjīsu katā,",
  "pli-tv-pvr12:7.4_0": "Alajjīsu evaṁ na vijjati;",
  "pli-tv-pvr12:7.5_0": "Bahumpi alajjī bhāseyya,",
  "pli-tv-pvr12:7.6_0": "Vattānusandhitena kāraye.",
  "pli-tv-pvr12:8.1_0": "Alajjī kīdiso hoti,",
  "pli-tv-pvr12:8.2_0": "Paṭiññā yassa na rūhati;",
  "pli-tv-pvr12:8.3_0": "Etañca tāhaṁ pucchāmi,",
  "pli-tv-pvr12:8.4_0": "Kīdiso vuccati alajjī puggalo.",
  "pli-tv-pvr12:9.1_0": "Sañcicca āpattiṁ āpajjati,",
  "pli-tv-pvr12:9.2_0": "Āpattiṁ parigūhati;",
  "pli-tv-pvr12:9.3_0": "Agatigamanañca gacchati,",
  "pli-tv-pvr12:9.4_0": "Ediso vuccati alajjīpuggalo.",
  "pli-tv-pvr12:10.1_0": "Saccaṁ ahampi jānāmi,",
  "pli-tv-pvr12:10.2_0": "Ediso vuccati alajjīpuggalo;",
  "pli-tv-pvr12:10.3_0": "Aññañca tāhaṁ pucchāmi,",
  "pli-tv-pvr12:10.4_0": "Kīdiso vuccati lajjīpuggalo.",
  "pli-tv-pvr12:11.1_0": "Sañcicca āpattiṁ nāpajjati,",
  "pli-tv-pvr12:11.2_0": "Āpattiṁ na parigūhati;",
  "pli-tv-pvr12:11.3_0": "Agatigamanaṁ na gacchati,",
  "pli-tv-pvr12:11.4_0": "Ediso vuccati lajjīpuggalo.",
  "pli-tv-pvr12:12.1_0": "Saccaṁ ahampi jānāmi,",
  "pli-tv-pvr12:12.2_0": "Ediso vuccati lajjīpuggalo;",
  "pli-tv-pvr12:12.3_0": "Aññañca tāhaṁ pucchāmi,",
  "pli-tv-pvr12:12.4_0": "Kīdiso vuccati adhammacodako.",
  "pli-tv-pvr12:13.1_0": "Akālena codeti abhūtena,",
  "pli-tv-pvr12:13.2_0": "Pharusena anatthasaṁhitena;",
  "pli-tv-pvr12:13.3_0": "Dosantaro codeti no mettācitto,",
  "pli-tv-pvr12:13.4_0": "Ediso vuccati adhammacodako.",
  "pli-tv-pvr12:14.1_0": "Saccaṁ ahampi jānāmi,",
  "pli-tv-pvr12:14.2_0": "Ediso vuccati adhammacodako;",
  "pli-tv-pvr12:14.3_0": "Aññañca tāhaṁ pucchāmi,",
  "pli-tv-pvr12:14.4_0": "Kīdiso vuccati dhammacodako.",
  "pli-tv-pvr12:15.1_0": "Kālena codeti bhūtena,",
  "pli-tv-pvr12:15.2_0": "Saṇhena atthasaṁhitena;",
  "pli-tv-pvr12:15.3_0": "Mettācitto codeti no dosantaro,",
  "pli-tv-pvr12:15.4_0": "Ediso vuccati dhammacodako.",
  "pli-tv-pvr12:16.1_0": "Saccaṁ ahampi jānāmi,",
  "pli-tv-pvr12:16.2_0": "Ediso vuccati dhammacodako;",
  "pli-tv-pvr12:16.3_0": "Aññañca tāhaṁ pucchāmi,",
  "pli-tv-pvr12:16.4_0": "Kīdiso vuccati bālacodako.",
  "pli-tv-pvr12:17.1_0": "Pubbāparaṁ na jānāti,",
  "pli-tv-pvr12:17.2_0": "Pubbāparassa akovido;",
  "pli-tv-pvr12:17.3_0": "Anusandhivacanapathaṁ na jānāti,",
  "pli-tv-pvr12:17.4_0": "Anusandhivacanapathassa akovido;",
  "pli-tv-pvr12:17.5_0": "Ediso vuccati bālacodako.",
  "pli-tv-pvr12:18.1_0": "Saccaṁ ahampi jānāmi,",
  "pli-tv-pvr12:18.2_0": "Ediso vuccati bālacodako;",
  "pli-tv-pvr12:18.3_0": "Aññañca tāhaṁ pucchāmi,",
  "pli-tv-pvr12:18.4_0": "Kīdiso vuccati paṇḍitacodako.",
  "pli-tv-pvr12:19.1_0": "Pubbāparampi jānāti,",
  "pli-tv-pvr12:19.2_0": "Pubbāparassa kovido;",
  "pli-tv-pvr12:19.3_0": "Anusandhivacanapathaṁ jānāti,",
  "pli-tv-pvr12:19.4_0": "Anusandhivacanapathassa kovido;",
  "pli-tv-pvr12:19.5_0": "Ediso vuccati paṇḍitacodako.",
  "pli-tv-pvr12:20.1_0": "Saccaṁ ahampi jānāmi,",
  "pli-tv-pvr12:20.2_0": "Ediso vuccati paṇḍitacodako;",
  "pli-tv-pvr12:20.3_0": "Aññañca tāhaṁ pucchāmi,",
  "pli-tv-pvr12:20.4_0": "Codanā kinti vuccati.",
  "pli-tv-pvr12:21.1_0": "Sīlavipattiyā codeti,",
  "pli-tv-pvr12:21.2_0": "atho ācāradiṭṭhiyā;",
  "pli-tv-pvr12:21.3_0": "Ājīvenapi codeti,",
  "pli-tv-pvr12:21.4_0": "codanā tena vuccatīti.",
  "pli-tv-pvr12:22.1_0": "Aparaṁ gāthāsaṅgaṇikaṁ niṭṭhitaṁ."
}