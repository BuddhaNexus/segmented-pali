{
  "ja515:0.1_0": "Jātaka",
  "ja515:0.2_0": "Tiṁsanipāta",
  "ja515:0.3_0": "Kiṁchandavagga",
  "ja515:0.4_0": "5. Sambhavajātaka",
  "ja515:1.1_0": "“Rajjañca paṭipannāsma,",
  "ja515:1.2_0": "ādhipaccaṁ sucīrata;",
  "ja515:1.3_0": "Mahattaṁ pattumicchāmi,",
  "ja515:1.4_0": "vijetuṁ pathaviṁ imaṁ.",
  "ja515:2.1_0": "Dhammena no adhammena,",
  "ja515:2.2_0": "adhammo me na ruccati;",
  "ja515:2.3_0": "Kiccova dhammo carito,",
  "ja515:2.4_0": "rañño hoti sucīrata.",
  "ja515:3.1_0": "Idha cevāninditā yena,",
  "ja515:3.2_0": "pecca yena aninditā;",
  "ja515:3.3_0": "Yasaṁ devamanussesu,",
  "ja515:3.4_0": "yena pappomu brāhmaṇa.",
  "ja515:4.1_0": "Yohaṁ atthañca dhammañca,",
  "ja515:4.2_0": "kattumicchāmi brāhmaṇa;",
  "ja515:4.3_0": "Taṁ tvaṁ atthañca dhammañca,",
  "ja515:4.4_0": "brāhmaṇakkhāhi pucchito”.",
  "ja515:5.1_0": "“Nāññatra vidhurā rāja,",
  "ja515:5.2_0": "etadakkhātumarahati;",
  "ja515:5.3_0": "Yaṁ tvaṁ atthañca dhammañca,",
  "ja515:5.4_0": "kattumicchasi khattiya”.",
  "ja515:6.1_0": "“Ehi kho pahito gaccha,",
  "ja515:6.2_0": "vidhurassa upantikaṁ;",
  "ja515:6.3_0": "Nikkhañcimaṁ suvaṇṇassa,",
  "ja515:6.4_0": "haraṁ gaccha sucīrata;",
  "ja515:6.5_0": "Abhihāraṁ imaṁ dajjā,",
  "ja515:6.6_0": "atthadhammānusiṭṭhiyā”.",
  "ja515:7.1_0": "Svādhippāgā bhāradvājo,",
  "ja515:7.2_0": "vidhurassa upantikaṁ;",
  "ja515:7.3_0": "Tamaddasa mahābrahmā,",
  "ja515:7.4_0": "asamānaṁ sake ghare.",
  "ja515:8.1_0": "“Raññohaṁ pahito dūto,",
  "ja515:8.2_0": "korabyassa yasassino;",
  "ja515:8.3_0": "‘Atthaṁ dhammañca pucchesi’,",
  "ja515:8.4_0": "iccabravi yudhiṭṭhilo;",
  "ja515:8.5_0": "Taṁ tvaṁ atthañca dhammañca,",
  "ja515:8.6_0": "vidhurakkhāhi pucchito”.",
  "ja515:9.1_0": "“Gaṅgaṁ me pidahissanti,",
  "ja515:9.2_0": "na taṁ sakkomi brāhmaṇa;",
  "ja515:9.3_0": "Apidhetuṁ mahāsindhuṁ,",
  "ja515:9.4_0": "taṁ kathaṁ so bhavissati;",
  "ja515:9.5_0": "Na te sakkomi akkhātuṁ,",
  "ja515:9.6_0": "atthaṁ dhammañca pucchito.",
  "ja515:10.1_0": "Bhadrakāro ca me putto,",
  "ja515:10.2_0": "oraso mama atrajo;",
  "ja515:10.3_0": "Taṁ tvaṁ atthañca dhammañca,",
  "ja515:10.4_0": "gantvā pucchassu brāhmaṇa”.",
  "ja515:11.1_0": "Svādhippāgā bhāradvājo,",
  "ja515:11.2_0": "bhadrakārassupantikaṁ;",
  "ja515:11.3_0": "Tamaddasa mahābrahmā,",
  "ja515:11.4_0": "nisinnaṁ samhi vesmani.",
  "ja515:12.1_0": "“Raññohaṁ pahito dūto,",
  "ja515:12.2_0": "korabyassa yasassino;",
  "ja515:12.3_0": "‘Atthaṁ dhammañca pucchesi’,",
  "ja515:12.4_0": "iccabravi yudhiṭṭhilo;",
  "ja515:12.5_0": "Taṁ tvaṁ atthañca dhammañca,",
  "ja515:12.6_0": "bhadrakāra pabrūhi me”.",
  "ja515:13.1_0": "“Maṁsakājaṁ avahāya,",
  "ja515:13.2_0": "godhaṁ anupatāmahaṁ;",
  "ja515:13.3_0": "Na te sakkomi akkhātuṁ,",
  "ja515:13.4_0": "atthaṁ dhammañca pucchito.",
  "ja515:14.1_0": "Sañcayo nāma me bhātā,",
  "ja515:14.2_0": "kaniṭṭho me sucīrata;",
  "ja515:14.3_0": "Taṁ tvaṁ atthañca dhammañca,",
  "ja515:14.4_0": "gantvā pucchassu brāhmaṇa”.",
  "ja515:15.1_0": "Svādhippāgā bhāradvājo,",
  "ja515:15.2_0": "sañcayassa upantikaṁ;",
  "ja515:15.3_0": "Tamaddasa mahābrahmā,",
  "ja515:15.4_0": "nisinnaṁ samhi vesmani.",
  "ja515:16.1_0": "“Raññohaṁ pahito dūto,",
  "ja515:16.2_0": "korabyassa yasassino;",
  "ja515:16.3_0": "‘Atthaṁ dhammañca pucchesi’,",
  "ja515:16.4_0": "iccabravi yudhiṭṭhilo;",
  "ja515:16.5_0": "Taṁ tvaṁ atthañca dhammañca,",
  "ja515:16.6_0": "sañcayakkhāhi pucchito”.",
  "ja515:17.1_0": "“Sadā maṁ gilate maccu,",
  "ja515:17.2_0": "sāyaṁ pāto sucīrata;",
  "ja515:17.3_0": "Na te sakkomi akkhātuṁ,",
  "ja515:17.4_0": "atthaṁ dhammañca pucchito.",
  "ja515:18.1_0": "Sambhavo nāma me bhātā,",
  "ja515:18.2_0": "kaniṭṭho me sucīrata;",
  "ja515:18.3_0": "Taṁ tvaṁ atthañca dhammañca,",
  "ja515:18.4_0": "gantvā pucchassu brāhmaṇa”.",
  "ja515:19.1_0": "“Abbhuto vata bho dhammo,",
  "ja515:19.2_0": "nāyaṁ asmāka ruccati;",
  "ja515:19.3_0": "Tayo janā pitāputtā,",
  "ja515:19.4_0": "te su paññāya no vidū.",
  "ja515:20.1_0": "Na taṁ sakkotha akkhātuṁ,",
  "ja515:20.2_0": "atthaṁ dhammañca pucchitā;",
  "ja515:20.3_0": "Kathaṁ nu daharo jaññā,",
  "ja515:20.4_0": "atthaṁ dhammañca pucchito”.",
  "ja515:21.1_0": "“Mā naṁ daharoti uññāsi,",
  "ja515:21.2_0": "apucchitvāna sambhavaṁ;",
  "ja515:21.3_0": "Pucchitvā sambhavaṁ jaññā,",
  "ja515:21.4_0": "atthaṁ dhammañca brāhmaṇa.",
  "ja515:22.1_0": "Yathāpi cando vimalo,",
  "ja515:22.2_0": "gacchaṁ ākāsadhātuyā;",
  "ja515:22.3_0": "Sabbe tārāgaṇe loke,",
  "ja515:22.4_0": "ābhāya atirocati.",
  "ja515:23.1_0": "Evampi daharūpeto,",
  "ja515:23.2_0": "paññāyogena sambhavo;",
  "ja515:23.3_0": "Mā naṁ daharoti uññāsi,",
  "ja515:23.4_0": "apucchitvāna sambhavaṁ;",
  "ja515:23.5_0": "Pucchitvā sambhavaṁ jaññā,",
  "ja515:23.6_0": "atthaṁ dhammañca brāhmaṇa.",
  "ja515:24.1_0": "Yathāpi rammako māso,",
  "ja515:24.2_0": "gimhānaṁ hoti brāhmaṇa;",
  "ja515:24.3_0": "Atevaññehi māsehi,",
  "ja515:24.4_0": "dumapupphehi sobhati.",
  "ja515:25.1_0": "Evampi daharūpeto,",
  "ja515:25.2_0": "paññāyogena sambhavo;",
  "ja515:25.3_0": "Mā naṁ daharoti uññāsi,",
  "ja515:25.4_0": "apucchitvāna sambhavaṁ;",
  "ja515:25.5_0": "Pucchitvā sambhavaṁ jaññā,",
  "ja515:25.6_0": "atthaṁ dhammañca brāhmaṇa.",
  "ja515:26.1_0": "Yathāpi himavā brahme,",
  "ja515:26.2_0": "pabbato gandhamādano;",
  "ja515:26.3_0": "Nānārukkhehi sañchanno,",
  "ja515:26.4_0": "mahābhūtagaṇālayo;",
  "ja515:26.5_0": "Osadhehi ca dibbehi,",
  "ja515:26.6_0": "disā bhāti pavāti ca.",
  "ja515:27.1_0": "Evampi daharūpeto,",
  "ja515:27.2_0": "paññāyogena sambhavo;",
  "ja515:27.3_0": "Mā naṁ daharoti uññāsi,",
  "ja515:27.4_0": "apucchitvāna sambhavaṁ;",
  "ja515:27.5_0": "Pucchitvā sambhavaṁ jaññā,",
  "ja515:27.6_0": "atthaṁ dhammañca brāhmaṇa.",
  "ja515:28.1_0": "Yathāpi pāvako brahme,",
  "ja515:28.2_0": "accimālī yasassimā;",
  "ja515:28.3_0": "Jalamāno vane gacche,",
  "ja515:28.4_0": "analo kaṇhavattanī.",
  "ja515:29.1_0": "Ghatāsano dhūmaketu,",
  "ja515:29.2_0": "uttamāhevanandaho;",
  "ja515:29.3_0": "Nisīthe pabbataggasmiṁ,",
  "ja515:29.4_0": "pahūtedho virocati.",
  "ja515:30.1_0": "Evampi daharūpeto,",
  "ja515:30.2_0": "paññāyogena sambhavo;",
  "ja515:30.3_0": "Mā naṁ daharoti uññāsi,",
  "ja515:30.4_0": "apucchitvāna sambhavaṁ;",
  "ja515:30.5_0": "Pucchitvā sambhavaṁ jaññā,",
  "ja515:30.6_0": "atthaṁ dhammañca brāhmaṇa.",
  "ja515:31.1_0": "Javena bhadraṁ jānanti,",
  "ja515:31.2_0": "balībaddañca vāhiye;",
  "ja515:31.3_0": "Dohena dhenuṁ jānanti,",
  "ja515:31.4_0": "bhāsamānañca paṇḍitaṁ.",
  "ja515:32.1_0": "Evampi daharūpeto,",
  "ja515:32.2_0": "paññāyogena sambhavo;",
  "ja515:32.3_0": "Mā naṁ daharoti uññāsi,",
  "ja515:32.4_0": "apucchitvāna sambhavaṁ;",
  "ja515:32.5_0": "Pucchitvā sambhavaṁ jaññā,",
  "ja515:32.6_0": "atthaṁ dhammañca brāhmaṇa”.",
  "ja515:33.1_0": "Svādhippāgā bhāradvājo,",
  "ja515:33.2_0": "sambhavassa upantikaṁ;",
  "ja515:33.3_0": "Tamaddasa mahābrahmā,",
  "ja515:33.4_0": "kīḷamānaṁ bahīpure.",
  "ja515:34.1_0": "“Raññohaṁ pahito dūto,",
  "ja515:34.2_0": "korabyassa yasassino;",
  "ja515:34.3_0": "‘Atthaṁ dhammañca pucchesi’,",
  "ja515:34.4_0": "iccabravi yudhiṭṭhilo;",
  "ja515:34.5_0": "Taṁ tvaṁ atthañca dhammañca,",
  "ja515:34.6_0": "sambhavakkhāhi pucchito”.",
  "ja515:35.1_0": "“Taggha te ahamakkhissaṁ,",
  "ja515:35.2_0": "yathāpi kusalo tathā;",
  "ja515:35.3_0": "Rājā ca kho taṁ jānāti,",
  "ja515:35.4_0": "yadi kāhati vā na vā.",
  "ja515:36.1_0": "‘Ajja suve’ti saṁseyya,",
  "ja515:36.2_0": "raññā puṭṭho sucīrata;",
  "ja515:36.3_0": "Mā katvā avasī rājā,",
  "ja515:36.4_0": "atthe jāte yudhiṭṭhilo.",
  "ja515:37.1_0": "Ajjhattaññeva saṁseyya,",
  "ja515:37.2_0": "raññā puṭṭho sucīrata;",
  "ja515:37.3_0": "Kummaggaṁ na niveseyya,",
  "ja515:37.4_0": "yathā mūḷho acetaso.",
  "ja515:38.1_0": "Attānaṁ nātivatteyya,",
  "ja515:38.2_0": "adhammaṁ na samācare;",
  "ja515:38.3_0": "Atitthe nappatāreyya,",
  "ja515:38.4_0": "anatthe na yuto siyā.",
  "ja515:39.1_0": "Yo ca etāni ṭhānāni,",
  "ja515:39.2_0": "kattuṁ jānāti khattiyo;",
  "ja515:39.3_0": "Sadā so vaḍḍhate rājā,",
  "ja515:39.4_0": "sukkapakkheva candimā.",
  "ja515:40.1_0": "Ñātīnañca piyo hoti,",
  "ja515:40.2_0": "mittesu ca virocati;",
  "ja515:40.3_0": "Kāyassa bhedā sappañño,",
  "ja515:40.4_0": "saggaṁ so upapajjatī”ti.",
  "ja515:41.1_0": "Sambhavajātakaṁ pañcamaṁ."
}