{
  "ja56:0.1_0": "Jātaka",
  "ja56:0.2_0": "Ekakanipāta",
  "ja56:0.3_0": "Āsīsavagga",
  "ja56:0.4_0": "6. Kañcanakkhandhajātaka",
  "ja56:1.1_0": "“Yo pahaṭṭhena cittena,",
  "ja56:1.2_0": "pahaṭṭhamanaso naro;",
  "ja56:1.3_0": "Bhāveti kusalaṁ dhammaṁ,",
  "ja56:1.4_0": "yogakkhemassa pattiyā;",
  "ja56:1.5_0": "Pāpuṇe anupubbena,",
  "ja56:1.6_0": "sabbasaṁyojanakkhayan”ti.",
  "ja56:2.1_0": "Kañcanakkhandhajātakaṁ chaṭṭhaṁ."
}