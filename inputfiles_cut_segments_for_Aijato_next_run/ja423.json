{
  "ja423:0.1_0": "Jātaka",
  "ja423:0.2_0": "Aṭṭhakanipāta",
  "ja423:0.3_0": "Kaccānivagga",
  "ja423:0.4_0": "7. Indriyajātaka",
  "ja423:1.1_0": "“Yo indriyānaṁ kāmena,",
  "ja423:1.2_0": "vasaṁ nārada gacchati;",
  "ja423:1.3_0": "So pariccajjubho loke,",
  "ja423:1.4_0": "jīvantova visussati.",
  "ja423:2.1_0": "Sukhassānantaraṁ dukkhaṁ,",
  "ja423:2.2_0": "dukkhassānantaraṁ sukhaṁ;",
  "ja423:2.3_0": "Sosi patto sukhā dukkhaṁ,",
  "ja423:2.4_0": "pāṭikaṅkha varaṁ sukhaṁ.",
  "ja423:3.1_0": "Kicchakāle kicchasaho,",
  "ja423:3.2_0": "yo kicchaṁ nātivattati;",
  "ja423:3.3_0": "Sa kicchantaṁ sukhaṁ dhīro,",
  "ja423:3.4_0": "yogaṁ samadhigacchati.",
  "ja423:4.1_0": "Na heva kāmāna kāmā,",
  "ja423:4.2_0": "nānatthā nātthakāraṇā;",
  "ja423:4.3_0": "Na katañca niraṅkatvā,",
  "ja423:4.4_0": "dhammā cavitumarahasi”.",
  "ja423:5.1_0": "“Dakkhaṁ gahapatī sādhu,",
  "ja423:5.2_0": "saṁvibhajjañca bhojanaṁ;",
  "ja423:5.3_0": "Ahāso atthalābhesu,",
  "ja423:5.4_0": "atthabyāpatti abyatho”.",
  "ja423:6.1_0": "“Ettāvatetaṁ paṇḍiccaṁ,",
  "ja423:6.2_0": "api so devilo bravi;",
  "ja423:6.3_0": "Na yito kiñci pāpiyo,",
  "ja423:6.4_0": "yo indriyānaṁ vasaṁ vaje”.",
  "ja423:7.1_0": "“Amittānaṁva hatthatthaṁ,",
  "ja423:7.2_0": "sivi pappoti māmiva;",
  "ja423:7.3_0": "Kammaṁ vijjañca dakkheyyaṁ,",
  "ja423:7.4_0": "vivāhaṁ sīlamaddavaṁ;",
  "ja423:7.5_0": "Ete ca yase hāpetvā,",
  "ja423:7.6_0": "nibbatto sehi kammehi.",
  "ja423:8.1_0": "Sohaṁ sahassajīnova,",
  "ja423:8.2_0": "abandhu aparāyaṇo;",
  "ja423:8.3_0": "Ariyadhammā apakkanto,",
  "ja423:8.4_0": "yathā peto tathevahaṁ.",
  "ja423:9.1_0": "Sukhakāme dukkhāpetvā,",
  "ja423:9.2_0": "āpannosmi padaṁ imaṁ;",
  "ja423:9.3_0": "So sukhaṁ nādhigacchāmi,",
  "ja423:9.4_0": "ṭhito bhāṇumatāmivā”ti.",
  "ja423:10.1_0": "Indriyajātakaṁ sattamaṁ."
}