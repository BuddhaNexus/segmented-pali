{
  "tha-ap227:0.1_0": "Therāpadāna",
  "tha-ap227:0.2_0": "Ālambaṇadāyakavagga",
  "tha-ap227:0.3_0": "5. Abyādhikattheraapadāna",
  "tha-ap227:1.1_0": "“Vipassissa bhagavato,",
  "tha-ap227:1.2_0": "aggisālaṁ adāsahaṁ;",
  "tha-ap227:1.3_0": "Byādhikānañca āvāsaṁ,",
  "tha-ap227:1.4_0": "uṇhodakapaṭiggahaṁ.",
  "tha-ap227:2.1_0": "Tena kammenayaṁ mayhaṁ,",
  "tha-ap227:2.2_0": "attabhāvo sunimmito;",
  "tha-ap227:2.3_0": "Byādhāhaṁ nābhijānāmi,",
  "tha-ap227:2.4_0": "puññakammassidaṁ phalaṁ.",
  "tha-ap227:3.1_0": "Ekanavutito kappe,",
  "tha-ap227:3.2_0": "yaṁ sālamadadiṁ tadā;",
  "tha-ap227:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap227:3.4_0": "aggisālāyidaṁ phalaṁ.",
  "tha-ap227:4.1_0": "Ito ca sattame kappe,",
  "tha-ap227:4.2_0": "ekosiṁ aparājito;",
  "tha-ap227:4.3_0": "Sattaratanasampanno,",
  "tha-ap227:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap227:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap227:5.2_0": "…pe…",
  "tha-ap227:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap227:6.1_0": "Itthaṁ sudaṁ āyasmā abyādhiko thero imā gāthāyo abhāsitthāti.",
  "tha-ap227:7.1_0": "Abyādhikattherassāpadānaṁ pañcamaṁ."
}