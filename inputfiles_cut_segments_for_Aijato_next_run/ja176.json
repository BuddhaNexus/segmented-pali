{
  "ja176:0.1_0": "Jātaka",
  "ja176:0.2_0": "Dukanipāta",
  "ja176:0.3_0": "Kalyāṇavagga",
  "ja176:0.4_0": "6. Kaḷāyamuṭṭhijātaka",
  "ja176:1.1_0": "“Bālo vatāyaṁ dumasākhagocaro,",
  "ja176:1.2_0": "Paññā janinda nayimassa vijjati;",
  "ja176:1.3_0": "Kaḷāyamuṭṭhiṁ avakiriya kevalaṁ,",
  "ja176:1.4_0": "Ekaṁ kaḷāyaṁ patitaṁ gavesati”.",
  "ja176:2.1_0": "“Evameva mayaṁ rāja,",
  "ja176:2.2_0": "ye caññe atilobhino;",
  "ja176:2.3_0": "Appena bahuṁ jiyyāma,",
  "ja176:2.4_0": "kaḷāyeneva vānaro”ti.",
  "ja176:3.1_0": "Kaḷāyamuṭṭhijātakaṁ chaṭṭhaṁ."
}