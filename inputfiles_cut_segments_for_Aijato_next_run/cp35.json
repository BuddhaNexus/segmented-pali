{
  "cp35:0.1_0": "Cariyāpiṭaka",
  "cp35:0.2_0": "Yudhañjayavagga",
  "cp35:0.3_0": "Upekkhāpāramī 1",
  "cp35:0.4_0": "Mahālomahaṁsacariya",
  "cp35:1.1_0": "“Susāne seyyaṁ kappemi,",
  "cp35:1.2_0": "Chavaṭṭhikaṁ upanidhāyahaṁ;",
  "cp35:1.3_0": "Gāmaṇḍalā upāgantvā,",
  "cp35:1.4_0": "Rūpaṁ dassentinappakaṁ.",
  "cp35:2.1_0": "Apare gandhamālañca,",
  "cp35:2.2_0": "bhojanaṁ vividhaṁ bahuṁ;",
  "cp35:2.3_0": "Upāyanānūpanenti,",
  "cp35:2.4_0": "haṭṭhā saṁviggamānasā.",
  "cp35:3.1_0": "Ye me dukkhaṁ upaharanti,",
  "cp35:3.2_0": "ye ca denti sukhaṁ mama;",
  "cp35:3.3_0": "Sabbesaṁ samako homi,",
  "cp35:3.4_0": "dayā kopo na vijjati.",
  "cp35:4.1_0": "Sukhadukkhe tulābhūto,",
  "cp35:4.2_0": "Yasesu ayasesu ca;",
  "cp35:4.3_0": "Sabbattha samako homi,",
  "cp35:4.4_0": "Esā me upekkhāpāramī”ti.",
  "cp35:5.1_0": "Mahālomahaṁsacariyaṁ pannarasamaṁ.",
  "cp35:6.1_0": "Yudhañjayavaggo tatiyo.",
  "cp35:7.1_0": "Tassuddānaṁ",
  "cp35:8.1_0": "Yudhañjayo somanasso,",
  "cp35:8.2_0": "ayogharabhisena ca;",
  "cp35:8.3_0": "Soṇanando mūgapakkho,",
  "cp35:8.4_0": "kapirājā saccasavhayo.",
  "cp35:9.1_0": "Vaṭṭako maccharājā ca,",
  "cp35:9.2_0": "kaṇhadīpāyano isi;",
  "cp35:9.3_0": "Sutasomo puna āsiṁ,",
  "cp35:9.4_0": "sāmo ca ekarājahu;",
  "cp35:9.5_0": "Upekkhāpāramī āsi,",
  "cp35:9.6_0": "iti vutthaṁ mahesinā.",
  "cp35:10.1_0": "Evaṁ bahubbidhaṁ dukkhaṁ,",
  "cp35:10.2_0": "sampattī ca bahubbidhā;",
  "cp35:10.3_0": "Bhavābhave anubhavitvā,",
  "cp35:10.4_0": "patto sambodhimuttamaṁ.",
  "cp35:11.1_0": "Datvā dātabbakaṁ dānaṁ,",
  "cp35:11.2_0": "sīlaṁ pūretvā asesato;",
  "cp35:11.3_0": "Nekkhamme pāramiṁ gantvā,",
  "cp35:11.4_0": "patto sambodhimuttamaṁ.",
  "cp35:12.1_0": "Paṇḍite paripucchitvā,",
  "cp35:12.2_0": "vīriyaṁ katvāna muttamaṁ;",
  "cp35:12.3_0": "Khantiyā pāramiṁ gantvā,",
  "cp35:12.4_0": "patto sambodhimuttamaṁ.",
  "cp35:13.1_0": "Katvā daḷhamadhiṭṭhānaṁ,",
  "cp35:13.2_0": "saccavācānurakkhiya;",
  "cp35:13.3_0": "Mettāya pāramiṁ gantvā,",
  "cp35:13.4_0": "patto sambodhimuttamaṁ.",
  "cp35:14.1_0": "Lābhālābhe yasāyase,",
  "cp35:14.2_0": "sammānanāvamānane;",
  "cp35:14.3_0": "Sabbattha samako hutvā,",
  "cp35:14.4_0": "patto sambodhimuttamaṁ.",
  "cp35:15.1_0": "Kosajjaṁ bhayato disvā,",
  "cp35:15.2_0": "vīriyārambhañca khemato;",
  "cp35:15.3_0": "Āraddhavīriyā hotha,",
  "cp35:15.4_0": "esā buddhānusāsanī.",
  "cp35:16.1_0": "Vivādaṁ bhayato disvā,",
  "cp35:16.2_0": "avivādañca khemato;",
  "cp35:16.3_0": "Samaggā sakhilā hotha,",
  "cp35:16.4_0": "esā buddhānusāsanī.",
  "cp35:17.1_0": "Pamādaṁ bhayato disvā,",
  "cp35:17.2_0": "appamādañca khemato;",
  "cp35:17.3_0": "Bhāvethaṭṭhaṅgikaṁ maggaṁ,",
  "cp35:17.4_0": "esā buddhānusāsanī.",
  "cp35:18.1_0": "Itthaṁ sudaṁ bhagavā attano pubbacariyaṁ sambhāvayamāno buddhāpadāniyaṁ nāma dhammapariyāyaṁ abhāsitthāti.",
  "cp35:19.1_0": "Cariyāpiṭakaṁ niṭṭhitaṁ."
}