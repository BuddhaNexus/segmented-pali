{
  "anya-e1210n0:0_0": "Milidaṭīkā",
  "anya-e1210n0:1_0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1210n0:2_0": "Nirantaraṁ lokahitassa kārakaṁ",
  "anya-e1210n0:3_0": "Nirantaraṁ lokahitassa desakaṁ,",
  "anya-e1210n0:4_0": "Nirantaraṁ lokahitassa cintakaṁ",
  "anya-e1210n0:5_0": "Namāmi vīraṁ naradammasārathiṁ;",
  "anya-e1210n0:6_0": "Pañhadhammaviduṁ nāthaṁ guyhadhammappakāsakaṁ,",
  "anya-e1210n0:7_0": "Namassivāna sambuddhaṁ dhammaṁ sādhuguṇampi ca;",
  "anya-e1210n0:8_0": "Nāgasenamahātheraṁ piṭakattayakovidaṁ,",
  "anya-e1210n0:9_0": "Vadivā tampi sirasā pañhadhammappakāsakaṁ;",
  "anya-e1210n0:10_0": "Milidapañhavivaraṇaṁ madhurathappakāsiniṁ,",
  "anya-e1210n0:11_0": "Racayissaṁ samāsena taṁ suṇātha samāhitā;",
  "anya-e1210n0:12_0": "1. Tatha pakiṇṇakathavivaraṇaṁ jātakuddharaṇanti dve yevamātikā.",
  "anya-e1210n0:13_0": "Tatha-",
  "anya-e1210n0:14_0": "Sambadho ca padañceva padatho padaviggaho",
  "anya-e1210n0:15_0": "Codanā parihāro’ti chabbidhā athavaṇṇanā’ti;",
  "anya-e1210n0:16_0": "Vuttattā sambadho tāva veditabbo. So ca yathāvuttājjhāhāravasena duvidho."
}