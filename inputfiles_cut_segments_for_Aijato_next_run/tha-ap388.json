{
  "tha-ap388:0.1_0": "Therāpadāna",
  "tha-ap388:0.2_0": "Avaṭaphalavagga",
  "tha-ap388:0.3_0": "6. Valliphaladāyakattheraapadāna",
  "tha-ap388:1.1_0": "“Sabbe janā samāgamma,",
  "tha-ap388:1.2_0": "agamiṁsu vanaṁ tadā;",
  "tha-ap388:1.3_0": "Phalamanvesamānā te,",
  "tha-ap388:1.4_0": "alabhiṁsu phalaṁ tadā.",
  "tha-ap388:2.1_0": "Tatthaddasāsiṁ sambuddhaṁ,",
  "tha-ap388:2.2_0": "sayambhuṁ aparājitaṁ;",
  "tha-ap388:2.3_0": "Pasannacitto sumano,",
  "tha-ap388:2.4_0": "valliphalamadāsahaṁ.",
  "tha-ap388:3.1_0": "Ekattiṁse ito kappe,",
  "tha-ap388:3.2_0": "yaṁ phalamadadiṁ tadā;",
  "tha-ap388:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap388:3.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap388:4.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap388:4.2_0": "…pe…",
  "tha-ap388:4.3_0": "viharāmi anāsavo.",
  "tha-ap388:5.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap388:5.2_0": "…pe…",
  "tha-ap388:5.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap388:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap388:6.2_0": "…pe…",
  "tha-ap388:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap388:7.1_0": "Itthaṁ sudaṁ āyasmā valliphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap388:8.1_0": "Valliphaladāyakattherassāpadānaṁ chaṭṭhaṁ."
}