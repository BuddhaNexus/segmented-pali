{
  "ja59:0.1_0": "Jātaka",
  "ja59:0.2_0": "Ekakanipāta",
  "ja59:0.3_0": "Āsīsavagga",
  "ja59:0.4_0": "9. Bherivādakajātaka",
  "ja59:1.1_0": "“Dhame dhame nātidhame,",
  "ja59:1.2_0": "atidhantañhi pāpakaṁ;",
  "ja59:1.3_0": "Dhantena hi sataṁ laddhaṁ,",
  "ja59:1.4_0": "atidhantena nāsitan”ti.",
  "ja59:2.1_0": "Bherivādakajātakaṁ navamaṁ."
}