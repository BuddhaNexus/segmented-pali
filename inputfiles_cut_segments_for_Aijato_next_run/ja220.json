{
  "ja220:0.1_0": "Jātaka",
  "ja220:0.2_0": "Dukanipāta",
  "ja220:0.3_0": "Bīraṇathambhavagga",
  "ja220:0.4_0": "10. Dhammadhajajātaka",
  "ja220:1.1_0": "“Sukhaṁ jīvitarūposi,",
  "ja220:1.2_0": "raṭṭhā vivanamāgato;",
  "ja220:1.3_0": "So ekako rukkhamūle,",
  "ja220:1.4_0": "kapaṇo viya jhāyasi”.",
  "ja220:2.1_0": "“Sukhaṁ jīvitarūposmi,",
  "ja220:2.2_0": "raṭṭhā vivanamāgato;",
  "ja220:2.3_0": "So ekako rukkhamūle,",
  "ja220:2.4_0": "kapaṇo viya jhāyāmi;",
  "ja220:2.5_0": "Sataṁ dhammaṁ anussaran”ti.",
  "ja220:3.1_0": "Dhammadhajajātakaṁ dasamaṁ.",
  "ja220:3.2_0": "Bīraṇathambhavaggo sattamo.",
  "ja220:4.1_0": "Tassuddānaṁ",
  "ja220:5.1_0": "Atha bīraṇathambhavaro ca naṭo,",
  "ja220:5.2_0": "Bharurājavaruttamapuṇṇanadī;",
  "ja220:5.3_0": "Bahubhāṇi aggipavane mūsikā,",
  "ja220:5.4_0": "Sahalambatthano kapaṇena dasāti."
}