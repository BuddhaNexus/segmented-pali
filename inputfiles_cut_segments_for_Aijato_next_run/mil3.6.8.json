{
  "mil3.6.8:0.1_0": "Milindapañha",
  "mil3.6.8:0.2_0": "Sativagga",
  "mil3.6.8:0.3_0": "8. Paññāpatiṭṭhānapañha",
  "mil3.6.8:1.1_0": "Rājā āha—",
  "mil3.6.8:1.2_0": "“bhante nāgasena, paññā kuhiṁ paṭivasatī”ti?",
  "mil3.6.8:1.3_0": "“Na katthaci, mahārājā”ti.",
  "mil3.6.8:1.4_0": "“Tena hi, bhante nāgasena, natthi paññā”ti.",
  "mil3.6.8:1.5_0": "“Vāto, mahārāja, kuhiṁ paṭivasatī”ti?",
  "mil3.6.8:1.6_0": "“Na katthaci, bhante”ti.",
  "mil3.6.8:1.7_0": "“Tena hi, mahārāja, natthi vāto”ti.",
  "mil3.6.8:2.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.6.8:3.1_0": "Paññāpatiṭṭhānapañho aṭṭhamo."
}