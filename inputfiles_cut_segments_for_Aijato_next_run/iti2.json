{
  "iti2:0.1_0": "Itivuttaka 2",
  "iti2:0.2_0": "Ekakanipāta",
  "iti2:0.3_0": "Paṭhamavagga",
  "iti2:0.4_0": "Dosasutta",
  "iti2:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti2:2.1_0": "“Ekadhammaṁ, bhikkhave, pajahatha;",
  "iti2:2.2_0": "ahaṁ vo pāṭibhogo anāgāmitāya.",
  "iti2:2.3_0": "Katamaṁ ekadhammaṁ?",
  "iti2:2.4_0": "Dosaṁ, bhikkhave, ekadhammaṁ pajahatha;",
  "iti2:2.5_0": "ahaṁ vo pāṭibhogo anāgāmitāyā”ti.",
  "iti2:2.6_0": "Etamatthaṁ bhagavā avoca.",
  "iti2:2.7_0": "Tatthetaṁ iti vuccati:",
  "iti2:3.1_0": "“Yena dosena duṭṭhāse,",
  "iti2:3.2_0": "sattā gacchanti duggatiṁ;",
  "iti2:3.3_0": "Taṁ dosaṁ sammadaññāya,",
  "iti2:3.4_0": "pajahanti vipassino;",
  "iti2:3.5_0": "Pahāya na punāyanti,",
  "iti2:3.6_0": "imaṁ lokaṁ kudācanan”ti.",
  "iti2:4.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti2:4.2_0": "Dutiyaṁ."
}