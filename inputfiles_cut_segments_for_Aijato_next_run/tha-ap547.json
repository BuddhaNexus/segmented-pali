{
  "tha-ap547:0.1_0": "Therāpadāna",
  "tha-ap547:0.2_0": "Bhaddiyavagga",
  "tha-ap547:0.3_0": "5. Nandakattheraapadāna",
  "tha-ap547:1.1_0": "“Padumuttaro nāma jino,",
  "tha-ap547:1.2_0": "sabbadhammesu cakkhumā;",
  "tha-ap547:1.3_0": "Ito satasahassamhi,",
  "tha-ap547:1.4_0": "kappe uppajji nāyako.",
  "tha-ap547:2.1_0": "Hitāya sabbasattānaṁ,",
  "tha-ap547:2.2_0": "sukhāya vadataṁ varo;",
  "tha-ap547:2.3_0": "Atthāya purisājañño,",
  "tha-ap547:2.4_0": "paṭipanno sadevake.",
  "tha-ap547:3.1_0": "Yasaggapatto sirimā,",
  "tha-ap547:3.2_0": "kittivaṇṇabhato jino;",
  "tha-ap547:3.3_0": "Pūjito sabbalokassa,",
  "tha-ap547:3.4_0": "disā sabbāsu vissuto.",
  "tha-ap547:4.1_0": "Uttiṇṇavicikiccho so,",
  "tha-ap547:4.2_0": "vītivattakathaṅkatho;",
  "tha-ap547:4.3_0": "Paripuṇṇamanasaṅkappo,",
  "tha-ap547:4.4_0": "patto sambodhimuttamaṁ.",
  "tha-ap547:5.1_0": "Anuppannassa maggassa,",
  "tha-ap547:5.2_0": "uppādetā naruttamo;",
  "tha-ap547:5.3_0": "Anakkhātañca akkhāsi,",
  "tha-ap547:5.4_0": "asañjātañca sañjanī.",
  "tha-ap547:6.1_0": "Maggaññū maggavidū ca,",
  "tha-ap547:6.2_0": "maggakkhāyī narāsabho;",
  "tha-ap547:6.3_0": "Maggassa kusalo satthā,",
  "tha-ap547:6.4_0": "sārathīnaṁ varuttamo.",
  "tha-ap547:7.1_0": "Tadā mahākāruṇiko,",
  "tha-ap547:7.2_0": "dhammaṁ desesi nāyako;",
  "tha-ap547:7.3_0": "Nimugge kāmapaṅkamhi,",
  "tha-ap547:7.4_0": "samuddharati pāṇine.",
  "tha-ap547:8.1_0": "Bhikkhunīnaṁ ovadane,",
  "tha-ap547:8.2_0": "sāvakaṁ seṭṭhasammataṁ;",
  "tha-ap547:8.3_0": "Vaṇṇayaṁ etadaggamhi,",
  "tha-ap547:8.4_0": "paññapesi mahāmuni.",
  "tha-ap547:9.1_0": "Taṁ sutvāhaṁ pamudito,",
  "tha-ap547:9.2_0": "nimantetvā tathāgataṁ;",
  "tha-ap547:9.3_0": "Bhojayitvā sasaṅghaṁ taṁ,",
  "tha-ap547:9.4_0": "patthayiṁ ṭhānamuttamaṁ.",
  "tha-ap547:10.1_0": "Tadā pamudito nātho,",
  "tha-ap547:10.2_0": "maṁ avoca mahāisi;",
  "tha-ap547:10.3_0": "‘Sukhī bhavassu dīghāvu,",
  "tha-ap547:10.4_0": "lacchase taṁ manorathaṁ.",
  "tha-ap547:11.1_0": "Satasahassito kappe,",
  "tha-ap547:11.2_0": "okkākakulasambhavo;",
  "tha-ap547:11.3_0": "Gotamo nāma gottena,",
  "tha-ap547:11.4_0": "satthā loke bhavissati.",
  "tha-ap547:12.1_0": "Tassa dhammesu dāyādo,",
  "tha-ap547:12.2_0": "oraso dhammanimmito;",
  "tha-ap547:12.3_0": "Nandako nāma nāmena,",
  "tha-ap547:12.4_0": "hessati satthu sāvako’.",
  "tha-ap547:13.1_0": "Tena kammena sukatena,",
  "tha-ap547:13.2_0": "cetanāpaṇidhīhi ca;",
  "tha-ap547:13.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "tha-ap547:13.4_0": "tāvatiṁsūpago ahaṁ.",
  "tha-ap547:14.1_0": "Pacchime ca bhave dāni,",
  "tha-ap547:14.2_0": "jāto seṭṭhikule ahaṁ;",
  "tha-ap547:14.3_0": "Sāvatthiyaṁ pure vare,",
  "tha-ap547:14.4_0": "iddhe phīte mahaddhane.",
  "tha-ap547:15.1_0": "Purappavese sugataṁ,",
  "tha-ap547:15.2_0": "disvā vimhitamānaso;",
  "tha-ap547:15.3_0": "Jetārāmapaṭiggāhe,",
  "tha-ap547:15.4_0": "pabbajiṁ anagāriyaṁ.",
  "tha-ap547:16.1_0": "Nacireneva kālena,",
  "tha-ap547:16.2_0": "arahattamapāpuṇiṁ;",
  "tha-ap547:16.3_0": "Tatohaṁ tiṇṇasaṁsāro,",
  "tha-ap547:16.4_0": "sāsito sabbadassinā.",
  "tha-ap547:17.1_0": "Bhikkhunīnaṁ dhammakathaṁ,",
  "tha-ap547:17.2_0": "paṭipucchākariṁ ahaṁ;",
  "tha-ap547:17.3_0": "Sāsitā tā mayā sabbā,",
  "tha-ap547:17.4_0": "abhaviṁsu anāsavā.",
  "tha-ap547:18.1_0": "Satāni pañcanūnāni,",
  "tha-ap547:18.2_0": "tadā tuṭṭho mahāhito;",
  "tha-ap547:18.3_0": "Bhikkhunīnaṁ ovadataṁ,",
  "tha-ap547:18.4_0": "aggaṭṭhāne ṭhapesi maṁ.",
  "tha-ap547:19.1_0": "Satasahasse kataṁ kammaṁ,",
  "tha-ap547:19.2_0": "phalaṁ dassesi me idha;",
  "tha-ap547:19.3_0": "Sumutto saravegova,",
  "tha-ap547:19.4_0": "kilese jhāpayiṁ mama.",
  "tha-ap547:20.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap547:20.2_0": "…pe…",
  "tha-ap547:20.3_0": "viharāmi anāsavo.",
  "tha-ap547:21.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap547:21.2_0": "…pe…",
  "tha-ap547:21.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap547:22.1_0": "Paṭisambhidā catasso,",
  "tha-ap547:22.2_0": "…pe…",
  "tha-ap547:22.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap547:23.1_0": "Itthaṁ sudaṁ āyasmā nandako thero imā gāthāyo abhāsitthāti.",
  "tha-ap547:24.1_0": "Nandakattherassāpadānaṁ pañcamaṁ."
}