{
  "tha-ap176:0.1_0": "Therāpadāna",
  "tha-ap176:0.2_0": "Kumudavagga",
  "tha-ap176:0.3_0": "4. Udapānadāyakattheraapadāna",
  "tha-ap176:1.1_0": "“Vipassino bhagavato,",
  "tha-ap176:1.2_0": "udapāno kato mayā;",
  "tha-ap176:1.3_0": "Piṇḍapātañca datvāna,",
  "tha-ap176:1.4_0": "niyyādesimahaṁ tadā.",
  "tha-ap176:2.1_0": "Ekanavutito kappe,",
  "tha-ap176:2.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap176:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap176:2.4_0": "udapānassidaṁ phalaṁ.",
  "tha-ap176:3.1_0": "Paṭisambhidā catasso,",
  "tha-ap176:3.2_0": "…pe…",
  "tha-ap176:3.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap176:4.1_0": "Itthaṁ sudaṁ āyasmā udapānadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap176:5.1_0": "Udapānadāyakattherassāpadānaṁ catutthaṁ."
}