{
  "ja485:0.1_0": "Jātaka",
  "ja485:0.2_0": "Pakiṇṇakanipāta",
  "ja485:0.3_0": "Sālikedāravagga",
  "ja485:0.4_0": "2. Candakinnarījātaka",
  "ja485:1.1_0": "“Upanīyatidaṁ maññe,",
  "ja485:1.2_0": "cande lohitamaddane;",
  "ja485:1.3_0": "Ajja jahāmi jīvitaṁ,",
  "ja485:1.4_0": "pāṇā me cande nirujjhanti.",
  "ja485:2.1_0": "Osīdi me dukkhaṁ hadayaṁ,",
  "ja485:2.2_0": "Me ḍayhate nitammāmi;",
  "ja485:2.3_0": "Tava candiyā socantiyā,",
  "ja485:2.4_0": "Na naṁ aññehi sokehi.",
  "ja485:3.1_0": "Tiṇamiva vanamiva milāyāmi,",
  "ja485:3.2_0": "Nadī aparipuṇṇāva sussāmi;",
  "ja485:3.3_0": "Tava candiyā socantiyā,",
  "ja485:3.4_0": "Na naṁ aññehi sokehi.",
  "ja485:4.1_0": "Vassamiva sare pāde,",
  "ja485:4.2_0": "Imāni assūni vattare mayhaṁ;",
  "ja485:4.3_0": "Tava candiyā socantiyā,",
  "ja485:4.4_0": "Na naṁ aññehi sokehi”.",
  "ja485:5.1_0": "“Pāpo khosi rājaputta,",
  "ja485:5.2_0": "Yo me icchitaṁ patiṁ varākiyā;",
  "ja485:5.3_0": "Vijjhasi vanamūlasmiṁ,",
  "ja485:5.4_0": "Soyaṁ viddho chamā seti.",
  "ja485:6.1_0": "Imaṁ mayhaṁ hadayasokaṁ,",
  "ja485:6.2_0": "Paṭimuñcatu rājaputta tava mātā;",
  "ja485:6.3_0": "Yo mayhaṁ hadayasoko,",
  "ja485:6.4_0": "Kimpurisaṁ avekkhamānāya.",
  "ja485:7.1_0": "Imaṁ mayhaṁ hadayasokaṁ,",
  "ja485:7.2_0": "Paṭimuñcatu rājaputta tava jāyā;",
  "ja485:7.3_0": "Yo mayhaṁ hadayasoko,",
  "ja485:7.4_0": "Kimpurisaṁ avekkhamānāya.",
  "ja485:8.1_0": "Mā ca puttaṁ mā ca patiṁ,",
  "ja485:8.2_0": "Addakkhi rājaputta tava mātā;",
  "ja485:8.3_0": "Yo kimpurisaṁ avadhi,",
  "ja485:8.4_0": "Adūsakaṁ mayha kāmā hi.",
  "ja485:9.1_0": "Mā ca puttaṁ mā ca patiṁ,",
  "ja485:9.2_0": "Addakkhi rājaputta tava jāyā;",
  "ja485:9.3_0": "Yo kimpurisaṁ avadhi,",
  "ja485:9.4_0": "Adūsakaṁ mayha kāmā hi”.",
  "ja485:10.1_0": "“Mā tvaṁ cande rodi,",
  "ja485:10.2_0": "Mā soci vanatimiramattakkhi;",
  "ja485:10.3_0": "Mama tvaṁ hehisi bhariyā,",
  "ja485:10.4_0": "Rājakule pūjitā nārībhi”.",
  "ja485:11.1_0": "“Api nūnahaṁ marissaṁ,",
  "ja485:11.2_0": "Nāhaṁ rājaputta tava hessaṁ;",
  "ja485:11.3_0": "Yo kimpurisaṁ avadhi,",
  "ja485:11.4_0": "Adūsakaṁ mayha kāmā hi”.",
  "ja485:12.1_0": "“Api bhīruke api jīvitukāmike,",
  "ja485:12.2_0": "Kimpurisi gaccha himavantaṁ;",
  "ja485:12.3_0": "Tālīsatagarabhojanā,",
  "ja485:12.4_0": "Aññe taṁ migā ramissanti”.",
  "ja485:13.1_0": "“Te pabbatā tā ca kandarā,",
  "ja485:13.2_0": "Tā ca giriguhāyo tatheva tiṭṭhanti;",
  "ja485:13.3_0": "Tattheva taṁ apassantī,",
  "ja485:13.4_0": "Kimpurisa kathaṁ ahaṁ kassaṁ.",
  "ja485:14.1_0": "Te paṇṇasanthatā ramaṇīyā,",
  "ja485:14.2_0": "Vāḷamigehi anuciṇṇā;",
  "ja485:14.3_0": "Tattheva taṁ apassantī,",
  "ja485:14.4_0": "Kimpurisa kathaṁ ahaṁ kassaṁ.",
  "ja485:15.1_0": "Te pupphasanthatā ramaṇīyā,",
  "ja485:15.2_0": "Vāḷamigehi anuciṇṇā;",
  "ja485:15.3_0": "Tattheva taṁ apassantī,",
  "ja485:15.4_0": "Kimpurisa kathaṁ ahaṁ kassaṁ.",
  "ja485:16.1_0": "Acchā savanti girivananadiyo,",
  "ja485:16.2_0": "Kusumābhikiṇṇasotāyo;",
  "ja485:16.3_0": "Tattheva taṁ apassantī,",
  "ja485:16.4_0": "Kimpurisa kathaṁ ahaṁ kassaṁ.",
  "ja485:17.1_0": "Nīlāni himavato pabbatassa,",
  "ja485:17.2_0": "Kūṭāni dassanīyāni;",
  "ja485:17.3_0": "Tattheva taṁ apassantī,",
  "ja485:17.4_0": "Kimpurisa kathaṁ ahaṁ kassaṁ.",
  "ja485:18.1_0": "Pītāni himavato pabbatassa,",
  "ja485:18.2_0": "Kūṭāni dassanīyāni;",
  "ja485:18.3_0": "Tattheva taṁ apassantī,",
  "ja485:18.4_0": "Kimpurisa kathaṁ ahaṁ kassaṁ.",
  "ja485:19.1_0": "Tambāni himavato pabbatassa,",
  "ja485:19.2_0": "Kūṭāni dassanīyāni;",
  "ja485:19.3_0": "Tattheva taṁ apassantī,",
  "ja485:19.4_0": "Kimpurisa kathaṁ ahaṁ kassaṁ.",
  "ja485:20.1_0": "Tuṅgāni himavato pabbatassa,",
  "ja485:20.2_0": "Kūṭāni dassanīyāni;",
  "ja485:20.3_0": "Tattheva taṁ apassantī,",
  "ja485:20.4_0": "Kimpurisa kathaṁ ahaṁ kassaṁ.",
  "ja485:21.1_0": "Setāni himavato pabbatassa,",
  "ja485:21.2_0": "Kūṭāni dassanīyāni;",
  "ja485:21.3_0": "Tattheva taṁ apassantī,",
  "ja485:21.4_0": "Kimpurisa kathaṁ ahaṁ kassaṁ.",
  "ja485:22.1_0": "Citrāni himavato pabbatassa,",
  "ja485:22.2_0": "Kūṭāni dassanīyāni;",
  "ja485:22.3_0": "Tattheva taṁ apassantī,",
  "ja485:22.4_0": "Kimpurisa kathaṁ ahaṁ kassaṁ.",
  "ja485:23.1_0": "Yakkhagaṇasevite gandhamādane,",
  "ja485:23.2_0": "Osadhebhi sañchanne;",
  "ja485:23.3_0": "Tattheva taṁ apassantī,",
  "ja485:23.4_0": "Kimpurisa kathaṁ ahaṁ kassaṁ.",
  "ja485:24.1_0": "Kimpurisasevite gandhamādane,",
  "ja485:24.2_0": "Osadhebhi sañchanne;",
  "ja485:24.3_0": "Tattheva taṁ apassantī,",
  "ja485:24.4_0": "Kimpurisa kathaṁ ahaṁ kassaṁ”.",
  "ja485:25.1_0": "“Vande te ayirabrahme,",
  "ja485:25.2_0": "Yo me icchitaṁ patiṁ varākiyā;",
  "ja485:25.3_0": "Amatena abhisiñci,",
  "ja485:25.4_0": "Samāgatāsmi piyatamena.",
  "ja485:26.1_0": "Vicarāma dāni girivana nadiyo,",
  "ja485:26.2_0": "Kusumābhikiṇṇasotāyo;",
  "ja485:26.3_0": "Nānādumavasanāyo,",
  "ja485:26.4_0": "Piyaṁvadā aññamaññassā”ti.",
  "ja485:27.1_0": "Candakinnarījātakaṁ dutiyaṁ."
}