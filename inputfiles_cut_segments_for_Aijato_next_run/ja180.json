{
  "ja180:0.1_0": "Jātaka",
  "ja180:0.2_0": "Dukanipāta",
  "ja180:0.3_0": "Kalyāṇavagga",
  "ja180:0.4_0": "10. Duddadajātaka",
  "ja180:1.1_0": "“Duddadaṁ dadamānānaṁ,",
  "ja180:1.2_0": "dukkaraṁ kamma kubbataṁ;",
  "ja180:1.3_0": "Asanto nānukubbanti,",
  "ja180:1.4_0": "sataṁ dhammo durannayo”.",
  "ja180:2.1_0": "“Tasmā satañca asataṁ,",
  "ja180:2.2_0": "nānā hoti ito gati;",
  "ja180:2.3_0": "Asanto nirayaṁ yanti,",
  "ja180:2.4_0": "santo saggaparāyaṇāti.",
  "ja180:3.1_0": "Duddadajātakaṁ dasamaṁ.",
  "ja180:3.2_0": "Kalyāṇavaggo tatiyo.",
  "ja180:4.1_0": "Tassuddānaṁ",
  "ja180:5.1_0": "Susamaññamigādhibhū māṇavako,",
  "ja180:5.2_0": "Vāripahūtarūpādiccupaṭṭhānā;",
  "ja180:5.3_0": "Sakaḷāyasatindukapaṅka puna,",
  "ja180:5.4_0": "Satadhamma sududdadakena dasā”ti."
}