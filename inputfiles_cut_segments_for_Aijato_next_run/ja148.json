{
  "ja148:0.1_0": "Jātaka",
  "ja148:0.2_0": "Ekakanipāta",
  "ja148:0.3_0": "Kakaṇṭakavagga",
  "ja148:0.4_0": "8. Siṅgālajātaka",
  "ja148:1.1_0": "“Nāhaṁ punaṁ na ca punaṁ,",
  "ja148:1.2_0": "na cāpi apunappunaṁ;",
  "ja148:1.3_0": "Hatthibondiṁ pavekkhāmi,",
  "ja148:1.4_0": "tathā hi bhayatajjito”ti.",
  "ja148:2.1_0": "Siṅgālajātakaṁ aṭṭhamaṁ."
}