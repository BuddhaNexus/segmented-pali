{
  "tika-vin08t35:0_0": "Nigamanakathā",
  "tika-vin08t35:1_0": "1.",
  "tika-vin08t35:2_0": "Jambudīpatale ramme, marammavisaye sute;",
  "tika-vin08t35:3_0": "Tambadīparaṭṭhe ṭhitaṁ, puraṁ ratananāmakaṁ.",
  "tika-vin08t35:4_0": "2.",
  "tika-vin08t35:5_0": "Jinasāsanapajjotaṁ, anekaratanākaraṁ;",
  "tika-vin08t35:6_0": "Sādhujjanānamāvāsaṁ, soṇṇapāsādalaṅkataṁ.",
  "tika-vin08t35:7_0": "3.",
  "tika-vin08t35:8_0": "Tasmiṁ ratanapuramhi, rājānekaraṭṭhissaro;",
  "tika-vin08t35:9_0": "Sirīsudhammarājāti, mahāadhipatīti ca.",
  "tika-vin08t35:10_0": "4.",
  "tika-vin08t35:11_0": "Evaṁnāmo mahātejo, rajjaṁ kāresi dhammato;",
  "tika-vin08t35:12_0": "Kārāpesi rājā maṇi-cūḷaṁ mahantacetiyaṁ.",
  "tika-vin08t35:13_0": "5.",
  "tika-vin08t35:14_0": "Tassa kāle brahāraññe, tiriyo nāma pabbato;",
  "tika-vin08t35:15_0": "Pubbakāraññavāsīnaṁ, nivāso bhāvanāraho.",
  "tika-vin08t35:16_0": "6.",
  "tika-vin08t35:17_0": "Aṭṭhārasahi dosehi, mutto pañcaṅgupāgato;",
  "tika-vin08t35:18_0": "Araññalakkhaṇaṁ patto, baddhasīmāyalaṅkato.",
  "tika-vin08t35:19_0": "7.",
  "tika-vin08t35:20_0": "Tasmiṁ pabbate vasanto, mahāthero supākaṭo;",
  "tika-vin08t35:21_0": "Tipeṭakālaṅkāroti, dvikkhattuṁ laddhalañchano.",
  "tika-vin08t35:22_0": "8.",
  "tika-vin08t35:23_0": "Tebhātukanarindānaṁ, garubhūto supesalo;",
  "tika-vin08t35:24_0": "Kusalo pariyattimhi, paṭipattimhi kārako.",
  "tika-vin08t35:25_0": "9.",
  "tika-vin08t35:26_0": "Sohaṁ lajjīpesalehi, bhikkhūhi abhiyācito;",
  "tika-vin08t35:27_0": "Sāsanassopakārāya, akāsiṁ sīlavaḍḍhanaṁ.",
  "tika-vin08t35:28_0": "10.",
  "tika-vin08t35:29_0": "Vinayālaṅkāraṁ nāma, lajjīnaṁ upakārakaṁ;",
  "tika-vin08t35:30_0": "Suṭṭhu vinayasaṅgaha-vaṇṇanaṁ sādhusevitaṁ.",
  "tika-vin08t35:31_0": "11.",
  "tika-vin08t35:32_0": "Rūpachiddanāsakaṇṇe, sampatte jinasāsane;",
  "tika-vin08t35:33_0": "Chiddasuññasuññarūpe, kaliyugamhi āgate.",
  "tika-vin08t35:34_0": "12.",
  "tika-vin08t35:35_0": "Niṭṭhāpitā ayaṁ ṭīkā, mayā sāsanakāraṇā;",
  "tika-vin08t35:36_0": "Dvīsu soṇṇavihāresu, dvikkhattuṁ laddhaketunā.",
  "tika-vin08t35:37_0": "13.",
  "tika-vin08t35:38_0": "Iminā puññakammena, aññena kusalena ca;",
  "tika-vin08t35:39_0": "Ito cutāhaṁ dutiye, attabhāvamhi āgate.",
  "tika-vin08t35:40_0": "14.",
  "tika-vin08t35:41_0": "Himavantapadesamhi, pabbate gandhamādane;",
  "tika-vin08t35:42_0": "Āsanne maṇiguhāya, mañjūsakadumassa ca.",
  "tika-vin08t35:43_0": "15.",
  "tika-vin08t35:44_0": "Tasmiṁ hessaṁ bhummadevo, atidīghāyuko varo;",
  "tika-vin08t35:45_0": "Paññāvīriyasampanno, buddhasāsanamāmako.",
  "tika-vin08t35:46_0": "16.",
  "tika-vin08t35:47_0": "Yāva tiṭṭhati sāsanaṁ, tāva cetiyavandanaṁ;",
  "tika-vin08t35:48_0": "Bodhipūjaṁ saṅghapūjaṁ, kareyyaṁ tuṭṭhamānaso.",
  "tika-vin08t35:49_0": "17.",
  "tika-vin08t35:50_0": "Bhikkhūnaṁ paṭipannānaṁ, veyyāvaccaṁ kareyyahaṁ;",
  "tika-vin08t35:51_0": "Pariyattābhiyuttānaṁ, kaṅkhāvinodayeyyahaṁ.",
  "tika-vin08t35:52_0": "18.",
  "tika-vin08t35:53_0": "Sāsanaṁ paggaṇhantānaṁ, rājūnaṁ sahāyo assaṁ;",
  "tika-vin08t35:54_0": "Sāsanaṁ niggaṇhantānaṁ, vāretuṁ samattho assaṁ.",
  "tika-vin08t35:55_0": "19.",
  "tika-vin08t35:56_0": "Sāsanantaradhāne tu, mañjūsaṁ rukkhamuttamaṁ;",
  "tika-vin08t35:57_0": "Nandamūlañca pabbhāraṁ, niccaṁ pūjaṁ kareyyahaṁ.",
  "tika-vin08t35:58_0": "20.",
  "tika-vin08t35:59_0": "Yadā tu paccekabuddhā, uppajjanti mahāyasā;",
  "tika-vin08t35:60_0": "Tadā tesaṁ niccakappaṁ, upaṭṭhānaṁ kareyyahaṁ.",
  "tika-vin08t35:61_0": "21.",
  "tika-vin08t35:62_0": "Teneva attabhāvena, yāva buddhuppādā ahaṁ;",
  "tika-vin08t35:63_0": "Tiṭṭhanto buddhuppādamhi, manussesu bhavāmahaṁ.",
  "tika-vin08t35:64_0": "22.",
  "tika-vin08t35:65_0": "Metteyyassa bhagavato, pabbajitvāna sāsane;",
  "tika-vin08t35:66_0": "Tosayitvāna jinaṁ taṁ, labhe byākaraṇuttamaṁ.",
  "tika-vin08t35:67_0": "23.",
  "tika-vin08t35:68_0": "Byākaraṇaṁ labhitvāna, pūretvā sabbapāramī;",
  "tika-vin08t35:69_0": "Anāgatamhi addhāne, buddho hessaṁ sadevaketi.",
  "tika-vin08t35:70_0": "Vinayālaṅkāraṭīkā samattā."
}