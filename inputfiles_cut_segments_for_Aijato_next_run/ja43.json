{
  "ja43:0.1_0": "Jātaka",
  "ja43:0.2_0": "Ekakanipāta",
  "ja43:0.3_0": "Atthakāmavagga",
  "ja43:0.4_0": "3. Veḷukajātaka",
  "ja43:1.1_0": "“Yo atthakāmassa hitānukampino,",
  "ja43:1.2_0": "Ovajjamāno na karoti sāsanaṁ;",
  "ja43:1.3_0": "Evaṁ so nihato seti,",
  "ja43:1.4_0": "Veḷukassa yathā pitā”ti.",
  "ja43:2.1_0": "Veḷukajātakaṁ tatiyaṁ."
}