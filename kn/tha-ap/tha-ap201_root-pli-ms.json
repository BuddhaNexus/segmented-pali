{
"tha-ap201:1.1": "Therāpadāna",
"tha-ap201:2.1": "Tamālapupphiyavagga",
"tha-ap201:3.1": "9 Kiṃkaṇikapupphiyattheraapadāna",
"tha-ap201:4.1": "“Sumaṅgaloti nāmena,",
"tha-ap201:4.2": "sayambhū aparājito;",
"tha-ap201:4.3": "Pavanā nikkhamitvāna,",
"tha-ap201:4.4": "nagaraṃ pāvisī jino.",
"tha-ap201:5.1": "Piṇḍacāraṃ caritvāna,",
"tha-ap201:5.2": "nikkhamma nagarā muni;",
"tha-ap201:5.3": "Katakiccova sambuddho,",
"tha-ap201:5.4": "so vasī vanamantare.",
"tha-ap201:6.1": "Kiṃkaṇipupphaṃ paggayha,",
"tha-ap201:6.2": "buddhassa abhiropayiṃ;",
"tha-ap201:6.3": "Pasannacitto sumano,",
"tha-ap201:6.4": "sayambhussa mahesino.",
"tha-ap201:7.1": "Catunnavutito kappe,",
"tha-ap201:7.2": "yaṃ pupphamabhiropayiṃ;",
"tha-ap201:7.3": "Duggatiṃ nābhijānāmi,",
"tha-ap201:7.4": "buddhapūjāyidaṃ phalaṃ.",
"tha-ap201:8.1": "Chaḷāsītimhito kappe,",
"tha-ap201:8.2": "apilāsisanāmako;",
"tha-ap201:8.3": "Sattaratanasampanno,",
"tha-ap201:8.4": "cakkavattī mahabbalo.",
"tha-ap201:9.1": "Paṭisambhidā catasso, … pe …",
"tha-ap201:9.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap201:10.1": "Itthaṃ sudaṃ āyasmā kiṃkaṇikapupphiyo thero imā gāthāyo abhāsitthāti.",
"tha-ap201:11.1": "Kiṃkaṇikapupphiyattherassāpadānaṃ navamaṃ."
}