{
"tha-ap112:1.1": "Therāpadāna",
"tha-ap112:2.1": "Bhikkhadāyivagga",
"tha-ap112:3.1": "10. Reṇupūjakattheraapadāna",
"tha-ap112:4.1": "“Suvaṇṇavaṇṇaṃ sambuddhaṃ,",
"tha-ap112:4.2": "Sataraṃsiṃva bhāṇumaṃ;",
"tha-ap112:4.3": "Obhāsentaṃ disā sabbā,",
"tha-ap112:4.4": "Uḷurājaṃva pūritaṃ.",
"tha-ap112:5.1": "Purakkhataṃ sāvakehi,",
"tha-ap112:5.2": "sāgareheva medaniṃ;",
"tha-ap112:5.3": "Nāgaṃ paggayha reṇūhi,",
"tha-ap112:5.4": "vipassissābhiropayiṃ.",
"tha-ap112:6.1": "Ekanavutito kappe,",
"tha-ap112:6.2": "yaṃ reṇumabhiropayiṃ;",
"tha-ap112:6.3": "Duggatiṃ nābhijānāmi,",
"tha-ap112:6.4": "buddhapūjāyidaṃ phalaṃ.",
"tha-ap112:7.1": "Paṇṇatālīsito kappe,",
"tha-ap112:7.2": "reṇu nāmāsi khattiyo;",
"tha-ap112:7.3": "Sattaratanasampanno,",
"tha-ap112:7.4": "cakkavattī mahabbalo.",
"tha-ap112:8.1": "Paṭisambhidā catasso, … pe …",
"tha-ap112:8.4": "kataṃ buddhassa sāsanaṃ”. #1668",
"tha-ap112:9.1": "Itthaṃ sudaṃ āyasmā reṇupūjako thero imā gāthāyo abhāsitthāti.",
"tha-ap112:10.1": "Reṇupūjakattherassāpadānaṃ dasamaṃ.",
"tha-ap112:11.1": "Bhikkhadāyivaggo ekādasamo.",
"tha-ap112:12.1": "Tassuddānaṃ",
"tha-ap112:13.1": "Bhikkhadāyī ñāṇasaññī,",
"tha-ap112:13.2": "hatthiyo padapūjako;",
"tha-ap112:13.3": "Muṭṭhipupphī udakado,",
"tha-ap112:13.4": "naḷamāli upaṭṭhako;",
"tha-ap112:13.5": "Biḷālidāyī reṇu ca,",
"tha-ap112:13.6": "gāthāyo cha ca saṭṭhi ca."
}