{
"tha-ap143:1.1": "Therāpadāna",
"tha-ap143:2.1": "Chattavagga",
"tha-ap143:3.1": "1 Atichattiyattheraapadāna",
"tha-ap143:4.1": "“Parinibbute bhagavati,",
"tha-ap143:4.2": "atthadassīnaruttame;",
"tha-ap143:4.3": "Chattātichattaṃ kāretvā,",
"tha-ap143:4.4": "thūpamhi abhiropayiṃ.",
"tha-ap143:5.1": "Kālena kālamāgantvā,",
"tha-ap143:5.2": "namassiṃ lokanāyakaṃ;",
"tha-ap143:5.3": "Pupphacchadanaṃ katvāna,",
"tha-ap143:5.4": "chattamhi abhiropayiṃ.",
"tha-ap143:6.1": "Sattarase kappasate,",
"tha-ap143:6.2": "devarajjamakārayiṃ;",
"tha-ap143:6.3": "Manussattaṃ na gacchāmi,",
"tha-ap143:6.4": "thūpapūjāyidaṃ phalaṃ.",
"tha-ap143:7.1": "Paṭisambhidā catasso,",
"tha-ap143:7.2": "vimokkhāpi ca aṭṭhime;",
"tha-ap143:7.3": "Chaḷabhiññā sacchikatā,",
"tha-ap143:7.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap143:8.1": "Itthaṃ sudaṃ āyasmā atichattiyo thero imā gāthāyo abhāsitthāti.",
"tha-ap143:9.1": "Atichattiyattherassāpadānaṃ paṭhamaṃ."
}