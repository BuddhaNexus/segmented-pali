{
"tha-ap292:1.1": "Therāpadāna",
"tha-ap292:2.1": "Paṇṇadāyakavagga",
"tha-ap292:3.1": "10 Pubbaṅgamiyattheraapadāna",
"tha-ap292:4.1": "“Cullāsītisahassāni,",
"tha-ap292:4.2": "pabbajimha akiñcanā;",
"tha-ap292:4.3": "Tesaṃ pubbaṅgamo āsiṃ,",
"tha-ap292:4.4": "uttamatthassa pattiyā.",
"tha-ap292:5.1": "Sarāgā sabhavā cete,",
"tha-ap292:5.2": "vippasannamanāvilā;",
"tha-ap292:5.3": "Upaṭṭhahiṃsu sakkaccaṃ,",
"tha-ap292:5.4": "pasannā sehi pāṇibhi.",
"tha-ap292:6.1": "Khīṇāsavā vantadosā,",
"tha-ap292:6.2": "katakiccā anāsavā;",
"tha-ap292:6.3": "Phariṃsu mettacittena,",
"tha-ap292:6.4": "sayambhū aparājitā.",
"tha-ap292:7.1": "Tesaṃ upaṭṭhahitvāna,",
"tha-ap292:7.2": "sambuddhānaṃ patissato;",
"tha-ap292:7.3": "Maraṇañca anuppatto,",
"tha-ap292:7.4": "devattañca agamhase.",
"tha-ap292:8.1": "Catunnavutito kappe,",
"tha-ap292:8.2": "yaṃ sīlamanupālayiṃ;",
"tha-ap292:8.3": "Duggatiṃ nābhijānāmi,",
"tha-ap292:8.4": "saññamassa idaṃ phalaṃ.",
"tha-ap292:9.1": "Paṭisambhidā catasso, … pe …",
"tha-ap292:9.4": "kataṃ buddhassa sāsanaṃ”. #2666",
"tha-ap292:10.1": "Itthaṃ sudaṃ āyasmā pubbaṅgamiyo thero imā gāthāyo abhāsitthāti.",
"tha-ap292:11.1": "Pubbaṅgamiyattherassāpadānaṃ dasamaṃ.",
"tha-ap292:12.1": "Paṇṇadāyakavaggo ekūnatiṃsatimo.",
"tha-ap292:13.1": "Tassuddānaṃ",
"tha-ap292:14.1": "Paṇṇaṃ phalaṃ paccuggamaṃ,",
"tha-ap292:14.2": "ekapupphi ca maghavā;",
"tha-ap292:14.3": "Upaṭṭhākāpadānañca,",
"tha-ap292:14.4": "pabbajjā buddhupaṭṭhāko;",
"tha-ap292:14.5": "Pubbaṅgamo ca gāthāyo,",
"tha-ap292:14.6": "aṭṭhatālīsa kittitā."
}