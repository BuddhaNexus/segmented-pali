{
"tha-ap263:1.1": "Therāpadāna",
"tha-ap263:2.1": "Padumukkhipavagga",
"tha-ap263:3.1": "1. Ākāsukkhipiyattheraapadāna",
"tha-ap263:4.1": "“Suvaṇṇavaṇṇaṃ siddhatthaṃ,",
"tha-ap263:4.2": "Gacchantaṃ antarāpaṇe;",
"tha-ap263:4.3": "Jalajagge duve gayha,",
"tha-ap263:4.4": "Upāgacchiṃ narāsabhaṃ.",
"tha-ap263:5.1": "Ekañca pupphaṃ pādesu,",
"tha-ap263:5.2": "buddhaseṭṭhassa nikkhipiṃ;",
"tha-ap263:5.3": "Ekañca pupphaṃ paggayha,",
"tha-ap263:5.4": "ākāse ukkhipiṃ ahaṃ.",
"tha-ap263:6.1": "Catunnavutito kappe,",
"tha-ap263:6.2": "yaṃ pupphamabhiropayiṃ;",
"tha-ap263:6.3": "Duggatiṃ nābhijānāmi,",
"tha-ap263:6.4": "pupphadānassidaṃ phalaṃ.",
"tha-ap263:7.1": "Ito chattiṃsakappamhi,",
"tha-ap263:7.2": "eko āsiṃ mahīpati;",
"tha-ap263:7.3": "Antalikkhakaro nāma,",
"tha-ap263:7.4": "cakkavattī mahabbalo.",
"tha-ap263:8.1": "Paṭisambhidā catasso,",
"tha-ap263:8.2": "vimokkhāpi ca aṭṭhime;",
"tha-ap263:8.3": "Chaḷabhiññā sacchikatā,",
"tha-ap263:8.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap263:9.1": "Itthaṃ sudaṃ āyasmā ākāsukkhipiyo thero imā gāthāyo abhāsitthāti.",
"tha-ap263:10.1": "Ākāsukkhipiyattherassāpadānaṃ paṭhamaṃ."
}