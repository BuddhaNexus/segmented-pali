{
"tha-ap155:1.1": "Therāpadāna",
"tha-ap155:2.1": "Bandhujīvakavagga",
"tha-ap155:3.1": "3. Vīthisammajjakattheraapadāna",
"tha-ap155:4.1": "“Udentaṃ sataraṃsiṃva,",
"tha-ap155:4.2": "pītaraṃsiṃva bhāṇumaṃ;",
"tha-ap155:4.3": "Pannarase yathā candaṃ,",
"tha-ap155:4.4": "niyyantaṃ lokanāyakaṃ.",
"tha-ap155:5.1": "Aṭṭhasaṭṭhisahassāni,",
"tha-ap155:5.2": "sabbe khīṇāsavā ahuṃ;",
"tha-ap155:5.3": "Parivāriṃsu sambuddhaṃ,",
"tha-ap155:5.4": "dvipadindaṃ narāsabhaṃ.",
"tha-ap155:6.1": "Sammajjitvāna taṃ vīthiṃ,",
"tha-ap155:6.2": "niyyante lokanāyake;",
"tha-ap155:6.3": "Ussāpesiṃ dhajaṃ tattha,",
"tha-ap155:6.4": "vippasannena cetasā.",
"tha-ap155:7.1": "Ekanavutito kappe,",
"tha-ap155:7.2": "yaṃ dhajaṃ abhiropayiṃ;",
"tha-ap155:7.3": "Duggatiṃ nābhijānāmi,",
"tha-ap155:7.4": "dhajadānassidaṃ phalaṃ.",
"tha-ap155:8.1": "Ito catutthake kappe,",
"tha-ap155:8.2": "rājāhosiṃ mahabbalo;",
"tha-ap155:8.3": "Sabbākārena sampanno,",
"tha-ap155:8.4": "sudhajo iti vissuto.",
"tha-ap155:9.1": "Paṭisambhidā catasso, … pe …",
"tha-ap155:9.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap155:10.1": "Itthaṃ sudaṃ āyasmā vīthisammajjako thero imā gāthāyo abhāsitthāti.",
"tha-ap155:11.1": "Vīthisammajjakattherassāpadānaṃ tatiyaṃ."
}