{
"tha-ap146:1.1": "Therāpadāna",
"tha-ap146:2.1": "Chattavagga",
"tha-ap146:3.1": "4. Saparivāriyattheraapadāna",
"tha-ap146:4.1": "“Padumuttaro nāma jino,",
"tha-ap146:4.2": "lokajeṭṭho narāsabho;",
"tha-ap146:4.3": "Jalitvā aggikkhandhova,",
"tha-ap146:4.4": "sambuddho parinibbuto.",
"tha-ap146:5.1": "Nibbute ca mahāvīre,",
"tha-ap146:5.2": "thūpo vitthāriko ahu;",
"tha-ap146:5.3": "Dūratova upaṭṭhenti,",
"tha-ap146:5.4": "dhātugehavaruttame.",
"tha-ap146:6.1": "Pasannacitto sumano,",
"tha-ap146:6.2": "akaṃ candanavedikaṃ;",
"tha-ap146:6.3": "Dissati thūpakhandho ca,",
"tha-ap146:6.4": "thūpānucchaviko tadā.",
"tha-ap146:7.1": "Bhave nibbattamānamhi,",
"tha-ap146:7.2": "devatte atha mānuse;",
"tha-ap146:7.3": "Omattaṃ me na passāmi,",
"tha-ap146:7.4": "pubbakammassidaṃ phalaṃ.",
"tha-ap146:8.1": "Pañcadasakappasate,",
"tha-ap146:8.2": "ito aṭṭha janā ahuṃ;",
"tha-ap146:8.3": "Sabbe samattanāmā te,",
"tha-ap146:8.4": "cakkavattī mahabbalā.",
"tha-ap146:9.1": "Paṭisambhidā catasso, … pe …",
"tha-ap146:9.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap146:10.1": "Itthaṃ sudaṃ āyasmā saparivāriyo thero imā gāthāyo abhāsitthāti.",
"tha-ap146:11.1": "Saparivāriyattherassāpadānaṃ catutthaṃ."
}