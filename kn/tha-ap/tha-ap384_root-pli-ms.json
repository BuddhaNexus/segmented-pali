{
"tha-ap384:1.1": "Therāpadāna",
"tha-ap384:2.1": "Avaṭaphalavagga",
"tha-ap384:3.1": "2. Labujadāyakattheraapadāna",
"tha-ap384:4.1": "“Nagare bandhumatiyā,",
"tha-ap384:4.2": "āsiṃ ārāmiko tadā;",
"tha-ap384:4.3": "Addasaṃ virajaṃ buddhaṃ,",
"tha-ap384:4.4": "gacchantaṃ anilañjase.",
"tha-ap384:5.1": "Labujassa phalaṃ gayha,",
"tha-ap384:5.2": "buddhaseṭṭhassadāsahaṃ;",
"tha-ap384:5.3": "Ākāse ṭhitako santo,",
"tha-ap384:5.4": "paṭigaṇhi mahāyaso.",
"tha-ap384:6.1": "Vittisañjananaṃ mayhaṃ,",
"tha-ap384:6.2": "diṭṭhadhammasukhāvahaṃ;",
"tha-ap384:6.3": "Phalaṃ buddhassa datvāna,",
"tha-ap384:6.4": "vippasannena cetasā.",
"tha-ap384:7.1": "Adhigacchiṃ tadā pītiṃ,",
"tha-ap384:7.2": "vipulañca sukhuttamaṃ;",
"tha-ap384:7.3": "Uppajjateva ratanaṃ,",
"tha-ap384:7.4": "nibbattassa tahiṃ tahiṃ.",
"tha-ap384:8.1": "Ekanavutito kappe,",
"tha-ap384:8.2": "yaṃ phalamadadiṃ tadā;",
"tha-ap384:8.3": "Duggatiṃ nābhijānāmi,",
"tha-ap384:8.4": "phaladānassidaṃ phalaṃ.",
"tha-ap384:9.1": "Kilesā jhāpitā mayhaṃ, … pe …",
"tha-ap384:9.4": "viharāmi anāsavo.",
"tha-ap384:10.1": "Svāgataṃ vata me āsi, … pe …",
"tha-ap384:10.4": "kataṃ buddhassa sāsanaṃ.",
"tha-ap384:11.1": "Paṭisambhidā catasso, … pe …",
"tha-ap384:11.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap384:12.1": "Itthaṃ sudaṃ āyasmā labujadāyako thero imā gāthāyo abhāsitthāti.",
"tha-ap384:13.1": "Labujadāyakattherassāpadānaṃ dutiyaṃ."
}