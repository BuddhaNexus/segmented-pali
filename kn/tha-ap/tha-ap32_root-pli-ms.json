{
"tha-ap32:1.1": "Therāpadāna",
"tha-ap32:2.1": "Subhūtivagga",
"tha-ap32:3.1": "10. Khomadāyakattheraapadāna",
"tha-ap32:4.1": "“Nagare bandhumatiyā,",
"tha-ap32:4.2": "ahosiṃ vāṇijo tadā;",
"tha-ap32:4.3": "Teneva dāraṃ posemi,",
"tha-ap32:4.4": "ropemi bījasampadaṃ.",
"tha-ap32:5.1": "Rathiyaṃ paṭipannassa,",
"tha-ap32:5.2": "vipassissa mahesino;",
"tha-ap32:5.3": "Ekaṃ khomaṃ mayā dinnaṃ,",
"tha-ap32:5.4": "kusalatthāya satthuno.",
"tha-ap32:6.1": "Ekanavutito kappe,",
"tha-ap32:6.2": "yaṃ khomamadadiṃ tadā;",
"tha-ap32:6.3": "Duggatiṃ nābhijānāmi,",
"tha-ap32:6.4": "khomadānassidaṃ phalaṃ.",
"tha-ap32:7.1": "Sattarase ito kappe,",
"tha-ap32:7.2": "eko sindhavasandhano;",
"tha-ap32:7.3": "Sattaratanasampanno,",
"tha-ap32:7.4": "catudīpamhi issaro.",
"tha-ap32:8.1": "Paṭisambhidā catasso,",
"tha-ap32:8.2": "Vimokkhāpi ca aṭṭhime;",
"tha-ap32:8.3": "Chaḷabhiññā sacchikatā,",
"tha-ap32:8.4": "Kataṃ buddhassa sāsanaṃ”. #991",
"tha-ap32:9.1": "Itthaṃ sudaṃ āyasmā khomadāyako thero imā gāthāyo abhāsitthāti.",
"tha-ap32:10.1": "Khomadāyakattherassāpadānaṃ dasamaṃ.",
"tha-ap32:11.1": "Tassuddānaṃ",
"tha-ap32:12.1": "Subhūti upavāno ca,",
"tha-ap32:12.2": "saraṇo sīlagāhako;",
"tha-ap32:12.3": "Annasaṃsāvako dhūpo,",
"tha-ap32:12.4": "pulino uttiyena ca.",
"tha-ap32:13.1": "Añjalī khomadāyī ca,",
"tha-ap32:13.2": "daseva tatiye gaṇe;",
"tha-ap32:13.3": "Pañcāsītisataṃ vuttā,",
"tha-ap32:13.4": "gāthāyo sabbapiṇḍitā.",
"tha-ap32:14.1": "Subhūtivaggo tatiyo.",
"tha-ap32:15.1": "Catutthabhāṇavāraṃ."
}