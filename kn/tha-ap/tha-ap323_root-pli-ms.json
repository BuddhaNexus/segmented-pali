{
"tha-ap323:1.1": "Therāpadāna",
"tha-ap323:2.1": "Umāpupphiyavagga",
"tha-ap323:3.1": "1 Umāpupphiyattheraapadāna",
"tha-ap323:4.1": "“Samāhitaṃ samāpannaṃ,",
"tha-ap323:4.2": "siddhatthamaparājitaṃ;",
"tha-ap323:4.3": "Samādhinā upaviṭṭhaṃ,",
"tha-ap323:4.4": "addasāhaṃ naruttamaṃ.",
"tha-ap323:5.1": "Umāpupphaṃ gahetvāna,",
"tha-ap323:5.2": "buddhassa abhiropayiṃ;",
"tha-ap323:5.3": "Sabbapupphā ekasīsā,",
"tha-ap323:5.4": "uddhaṃvaṇṭā adhomukhā.",
"tha-ap323:6.1": "Sucittā viya tiṭṭhante,",
"tha-ap323:6.2": "ākāse pupphasantharā;",
"tha-ap323:6.3": "Tena cittappasādena,",
"tha-ap323:6.4": "tusitaṃ upapajjahaṃ.",
"tha-ap323:7.1": "Catunnavutito kappe,",
"tha-ap323:7.2": "yaṃ pupphamabhipūjayiṃ;",
"tha-ap323:7.3": "Duggatiṃ nābhijānāmi,",
"tha-ap323:7.4": "buddhapūjāyidaṃ phalaṃ.",
"tha-ap323:8.1": "Pañcapaññāsito kappe,",
"tha-ap323:8.2": "eko āsiṃ mahīpati;",
"tha-ap323:8.3": "Samantachadano nāma,",
"tha-ap323:8.4": "cakkavattī mahabbalo.",
"tha-ap323:9.1": "Paṭisambhidā catasso,",
"tha-ap323:9.2": "vimokkhāpi ca aṭṭhime;",
"tha-ap323:9.3": "Chaḷabhiññā sacchikatā,",
"tha-ap323:9.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap323:10.1": "Itthaṃ sudaṃ āyasmā umāpupphiyo thero imā gāthāyo abhāsitthāti.",
"tha-ap323:11.1": "Umāpupphiyattherassāpadānaṃ paṭhamaṃ."
}