{
"tha-ap557:1.1": "Therāpadāna",
"tha-ap557:2.1": "Yasavagga",
"tha-ap557:3.1": "5. Vajjīputtattheraapadāna",
"tha-ap557:4.1": "“Sahassaraṃsī bhagavā,",
"tha-ap557:4.2": "sayambhū aparājito;",
"tha-ap557:4.3": "Vivekā vuṭṭhahitvāna,",
"tha-ap557:4.4": "gocarāyābhinikkhami.",
"tha-ap557:5.1": "Phalahattho ahaṃ disvā,",
"tha-ap557:5.2": "upagacchiṃ narāsabhaṃ;",
"tha-ap557:5.3": "Pasannacitto sumano,",
"tha-ap557:5.4": "savaṇṭaṃ adadiṃ phalaṃ.",
"tha-ap557:6.1": "Catunnavutito kappe,",
"tha-ap557:6.2": "yaṃ phalaṃ adadiṃ tadā;",
"tha-ap557:6.3": "Duggatiṃ nābhijānāmi,",
"tha-ap557:6.4": "phaladānassidaṃ phalaṃ.",
"tha-ap557:7.1": "Kilesā jhāpitā mayhaṃ, … pe …",
"tha-ap557:7.4": "viharāmi anāsavo.",
"tha-ap557:8.1": "Svāgataṃ vata me āsi, … pe …",
"tha-ap557:8.4": "kataṃ buddhassa sāsanaṃ.",
"tha-ap557:9.1": "Paṭisambhidā catasso, … pe …",
"tha-ap557:9.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap557:10.1": "Itthaṃ sudaṃ āyasmā vajjīputto thero imā gāthāyo abhāsitthāti.",
"tha-ap557:11.1": "Vajjīputtattherassāpadānaṃ pañcamaṃ."
}