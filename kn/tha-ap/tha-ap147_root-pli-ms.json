{
"tha-ap147:1.1": "Therāpadāna",
"tha-ap147:2.1": "Chattavagga",
"tha-ap147:3.1": "5 Umāpupphiyattheraapadāna",
"tha-ap147:4.1": "“Nibbute lokamahite,",
"tha-ap147:4.2": "āhutīnaṃ paṭiggahe;",
"tha-ap147:4.3": "Siddhatthamhi bhagavati,",
"tha-ap147:4.4": "mahāthūpamaho ahu.",
"tha-ap147:5.1": "Mahe pavattamānamhi,",
"tha-ap147:5.2": "siddhatthassa mahesino;",
"tha-ap147:5.3": "Umāpupphaṃ gahetvāna,",
"tha-ap147:5.4": "thūpamhi abhiropayiṃ.",
"tha-ap147:6.1": "Catunnavutito kappe,",
"tha-ap147:6.2": "yaṃ pupphamabhiropayiṃ;",
"tha-ap147:6.3": "Duggatiṃ nābhijānāmi,",
"tha-ap147:6.4": "thūpapūjāyidaṃ phalaṃ.",
"tha-ap147:7.1": "Ito ca navame kappe,",
"tha-ap147:7.2": "somadevasanāmakā;",
"tha-ap147:7.3": "Pañcāsītisu rājāno,",
"tha-ap147:7.4": "cakkavattī mahabbalā.",
"tha-ap147:8.1": "Paṭisambhidā catasso, … pe …",
"tha-ap147:8.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap147:9.1": "Itthaṃ sudaṃ āyasmā umāpupphiyo thero imā gāthāyo abhāsitthāti.",
"tha-ap147:10.1": "Umāpupphiyattherassāpadānaṃ pañcamaṃ."
}