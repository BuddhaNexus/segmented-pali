{
"tha-ap182:1.1": "Therāpadāna",
"tha-ap182:2.1": "Kumudavagga",
"tha-ap182:3.1": "10. Saṃghupaṭṭhākattheraapadāna",
"tha-ap182:4.1": "“Vessabhumhi bhagavati,",
"tha-ap182:4.2": "ahosārāmiko ahaṃ;",
"tha-ap182:4.3": "Pasannacitto sumano,",
"tha-ap182:4.4": "upaṭṭhiṃ saṃghamuttamaṃ.",
"tha-ap182:5.1": "Ekattiṃse ito kappe,",
"tha-ap182:5.2": "yaṃ kammamakariṃ tadā;",
"tha-ap182:5.3": "Duggatiṃ nābhijānāmi,",
"tha-ap182:5.4": "upaṭṭhānassidaṃ phalaṃ.",
"tha-ap182:6.1": "Ito te sattame kappe,",
"tha-ap182:6.2": "sattevāsuṃ samodakā;",
"tha-ap182:6.3": "Sattaratanasampannā,",
"tha-ap182:6.4": "cakkavattī mahabbalā.",
"tha-ap182:7.1": "Paṭisambhidā catasso, … pe …",
"tha-ap182:7.4": "kataṃ buddhassa sāsanaṃ”. #2146",
"tha-ap182:8.1": "Itthaṃ sudaṃ āyasmā saṃghupaṭṭhāko thero imā gāthāyo abhāsitthāti.",
"tha-ap182:9.1": "Saṃghupaṭṭhākattherassāpadānaṃ dasamaṃ.",
"tha-ap182:10.1": "Kumudavaggo aṭṭhārasamo.",
"tha-ap182:11.1": "Tassuddānaṃ",
"tha-ap182:12.1": "Kumudo atha nisseṇī,",
"tha-ap182:12.2": "rattiko udapānado;",
"tha-ap182:12.3": "Sīhāsanī maggadado,",
"tha-ap182:12.4": "ekadīpī maṇippado;",
"tha-ap182:12.5": "Tikicchako upaṭṭhāko,",
"tha-ap182:12.6": "ekapaññāsa gāthakāti."
}