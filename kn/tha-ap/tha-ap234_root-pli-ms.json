{
"tha-ap234:1.1": "Therāpadāna",
"tha-ap234:2.1": "Udakāsanavagga",
"tha-ap234:3.1": "2 Bhājanapālakattheraapadāna",
"tha-ap234:4.1": "“Nagare bandhumatiyā,",
"tha-ap234:4.2": "kumbhakāro ahaṃ tadā;",
"tha-ap234:4.3": "Bhājanaṃ anupālesiṃ,",
"tha-ap234:4.4": "bhikkhusaṃghassa tāvade.",
"tha-ap234:5.1": "Ekanavutito kappe,",
"tha-ap234:5.2": "bhājanaṃ anupālayiṃ;",
"tha-ap234:5.3": "Duggatiṃ nābhijānāmi,",
"tha-ap234:5.4": "bhājanassa idaṃ phalaṃ.",
"tha-ap234:6.1": "Tepaññāse ito kappe,",
"tha-ap234:6.2": "anantajālināmako;",
"tha-ap234:6.3": "Sattaratanasampanno,",
"tha-ap234:6.4": "cakkavattī mahabbalo.",
"tha-ap234:7.1": "Paṭisambhidā catasso, … pe …",
"tha-ap234:7.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap234:8.1": "Itthaṃ sudaṃ āyasmā bhājanapālako thero imā gāthāyo abhāsitthāti.",
"tha-ap234:9.1": "Bhājanapālakattherassāpadānaṃ dutiyaṃ."
}