{
"tha-ap130:1.1": "Therāpadāna",
"tha-ap130:2.1": "Sereyyavagga",
"tha-ap130:3.1": "8 Ñāṇasaññikattheraapadāna",
"tha-ap130:4.1": "“Pabbate himavantamhi,",
"tha-ap130:4.2": "vasāmi pabbatantare;",
"tha-ap130:4.3": "Pulinaṃ sobhanaṃ disvā,",
"tha-ap130:4.4": "buddhaseṭṭhaṃ anussariṃ.",
"tha-ap130:5.1": "Ñāṇe upanidhā natthi,",
"tha-ap130:5.2": "saṅkhāraṃ natthi satthuno;",
"tha-ap130:5.3": "Sabbadhammaṃ abhiññāya,",
"tha-ap130:5.4": "ñāṇena adhimuccati.",
"tha-ap130:6.1": "Namo te purisājañña,",
"tha-ap130:6.2": "namo te purisuttama;",
"tha-ap130:6.3": "Ñāṇena te samo natthi,",
"tha-ap130:6.4": "yāvatā ñāṇamuttamaṃ.",
"tha-ap130:7.1": "Ñāṇe cittaṃ pasādetvā,",
"tha-ap130:7.2": "kappaṃ saggamhi modahaṃ;",
"tha-ap130:7.3": "Avasesesu kappesu,",
"tha-ap130:7.4": "kusalaṃ caritaṃ mayā.",
"tha-ap130:8.1": "Ekanavutito kappe,",
"tha-ap130:8.2": "yaṃ saññamalabhiṃ tadā;",
"tha-ap130:8.3": "Duggatiṃ nābhijānāmi,",
"tha-ap130:8.4": "ñāṇasaññāyidaṃ phalaṃ.",
"tha-ap130:9.1": "Ito sattatikappamhi,",
"tha-ap130:9.2": "eko pulinapupphiyo;",
"tha-ap130:9.3": "Sattaratanasampanno,",
"tha-ap130:9.4": "cakkavattī mahabbalo.",
"tha-ap130:10.1": "Paṭisambhidā catasso, … pe …",
"tha-ap130:10.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap130:11.1": "Itthaṃ sudaṃ āyasmā ñāṇasaññiko thero imā gāthāyo abhāsitthāti.",
"tha-ap130:12.1": "Ñāṇasaññikattherassāpadānaṃ aṭṭhamaṃ."
}