{
"tha-ap273:1.1": "Therāpadāna",
"tha-ap273:2.1": "Suvaṇṇabibbohanavagga",
"tha-ap273:3.1": "1 Suvaṇṇabibbohaniyattheraapadāna",
"tha-ap273:4.1": "“Ekāsanaṃ ahamadaṃ,",
"tha-ap273:4.2": "pasanno sehi pāṇibhi;",
"tha-ap273:4.3": "Bibbohanañca pādāsiṃ,",
"tha-ap273:4.4": "uttamatthassa pattiyā.",
"tha-ap273:5.1": "Ekanavutito kappe,",
"tha-ap273:5.2": "bibbohanamadāsahaṃ;",
"tha-ap273:5.3": "Duggatiṃ nābhijānāmi,",
"tha-ap273:5.4": "bibbohanassidaṃ phalaṃ.",
"tha-ap273:6.1": "Ito tesaṭṭhime kappe,",
"tha-ap273:6.2": "asamo nāma khattiyo;",
"tha-ap273:6.3": "Sattaratanasampanno,",
"tha-ap273:6.4": "cakkavattī mahabbalo.",
"tha-ap273:7.1": "Paṭisambhidā catasso,",
"tha-ap273:7.2": "vimokkhāpi ca aṭṭhime;",
"tha-ap273:7.3": "Chaḷabhiññā sacchikatā,",
"tha-ap273:7.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap273:8.1": "Itthaṃ sudaṃ āyasmā suvaṇṇabibbohaniyo thero imā gāthāyo abhāsitthāti.",
"tha-ap273:9.1": "Suvaṇṇabibbohaniyattherassāpadānaṃ paṭhamaṃ."
}