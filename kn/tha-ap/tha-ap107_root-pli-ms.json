{
"tha-ap107:1.1": "Therāpadāna",
"tha-ap107:2.1": "Bhikkhadāyivagga",
"tha-ap107:3.1": "5. Muṭṭhipupphiyattheraapadāna",
"tha-ap107:4.1": "“Sudassanoti nāmena,",
"tha-ap107:4.2": "mālākāro ahaṃ tadā;",
"tha-ap107:4.3": "Addasaṃ virajaṃ buddhaṃ,",
"tha-ap107:4.4": "lokajeṭṭhaṃ narāsabhaṃ.",
"tha-ap107:5.1": "Jātipupphaṃ gahetvāna,",
"tha-ap107:5.2": "pūjayiṃ padumuttaraṃ;",
"tha-ap107:5.3": "Visuddhacakkhu sumano,",
"tha-ap107:5.4": "dibbacakkhuṃ samajjhagaṃ.",
"tha-ap107:6.1": "Etissā pupphapūjāya,",
"tha-ap107:6.2": "cittassa paṇidhīhi ca;",
"tha-ap107:6.3": "Kappānaṃ satasahassaṃ,",
"tha-ap107:6.4": "duggatiṃ nupapajjahaṃ.",
"tha-ap107:7.1": "Soḷasāsiṃsu rājāno,",
"tha-ap107:7.2": "devuttarasanāmakā;",
"tha-ap107:7.3": "Chattiṃsamhi ito kappe,",
"tha-ap107:7.4": "cakkavattī mahabbalā.",
"tha-ap107:8.1": "Paṭisambhidā catasso, … pe …",
"tha-ap107:8.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap107:9.1": "Itthaṃ sudaṃ āyasmā muṭṭhipupphiyo thero imā gāthāyo abhāsitthāti.",
"tha-ap107:10.1": "Muṭṭhipupphiyattherassāpadānaṃ pañcamaṃ."
}