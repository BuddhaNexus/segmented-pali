{
"tha-ap41:1.1": "Therāpadāna",
"tha-ap41:2.1": "Kuṇḍadhānavagga",
"tha-ap41:3.1": "9. Dhammacakkikattheraapadāna",
"tha-ap41:4.1": "“Siddhatthassa bhagavato,",
"tha-ap41:4.2": "sīhāsanassa sammukhā;",
"tha-ap41:4.3": "Dhammacakkaṃ me ṭhapitaṃ,",
"tha-ap41:4.4": "sukataṃ viññuvaṇṇitaṃ.",
"tha-ap41:5.1": "Cāruvaṇṇova sobhāmi,",
"tha-ap41:5.2": "sayoggabalavāhano;",
"tha-ap41:5.3": "Parivārenti maṃ niccaṃ,",
"tha-ap41:5.4": "anuyantā bahujjanā.",
"tha-ap41:6.1": "Saṭṭhitūriyasahassehi,",
"tha-ap41:6.2": "paricāremahaṃ sadā;",
"tha-ap41:6.3": "Parivārena sobhāmi,",
"tha-ap41:6.4": "puññakammassidaṃ phalaṃ.",
"tha-ap41:7.1": "Catunnavutito kappe,",
"tha-ap41:7.2": "yaṃ cakkaṃ ṭhapayiṃ ahaṃ;",
"tha-ap41:7.3": "Duggatiṃ nābhijānāmi,",
"tha-ap41:7.4": "dhammacakkassidaṃ phalaṃ.",
"tha-ap41:8.1": "Ito ekādase kappe,",
"tha-ap41:8.2": "aṭṭhāsiṃsu janādhipā;",
"tha-ap41:8.3": "Sahassarājanāmena,",
"tha-ap41:8.4": "cakkavattī mahabbalā.",
"tha-ap41:9.1": "Paṭisambhidā catasso, … pe …",
"tha-ap41:9.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap41:10.1": "Itthaṃ sudaṃ āyasmā dhammacakkiko thero imā gāthāyo abhāsitthāti.",
"tha-ap41:11.1": "Dhammacakkikattherassāpadānaṃ navamaṃ."
}