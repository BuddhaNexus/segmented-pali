{
"tha-ap353:1.1": "Therāpadāna",
"tha-ap353:2.1": "Saddasaññakavagga",
"tha-ap353:3.1": "1. Saddasaññakattheraapadāna",
"tha-ap353:4.1": "“Migaluddo pure āsiṃ,",
"tha-ap353:4.2": "araññe kānane ahaṃ;",
"tha-ap353:4.3": "Tatthaddasāsiṃ sambuddhaṃ,",
"tha-ap353:4.4": "devasaṅghapurakkhataṃ.",
"tha-ap353:5.1": "Catusaccaṃ pakāsentaṃ,",
"tha-ap353:5.2": "uddharantaṃ mahājanaṃ;",
"tha-ap353:5.3": "Assosiṃ madhuraṃ vācaṃ,",
"tha-ap353:5.4": "karavīkarudopamaṃ.",
"tha-ap353:6.1": "Brahmasarassa munino,",
"tha-ap353:6.2": "sikhino lokabandhuno;",
"tha-ap353:6.3": "Ghose cittaṃ pasādetvā,",
"tha-ap353:6.4": "pattomhi āsavakkhayaṃ.",
"tha-ap353:7.1": "Ekattiṃse ito kappe,",
"tha-ap353:7.2": "yaṃ kammamakariṃ tadā;",
"tha-ap353:7.3": "Duggatiṃ nābhijānāmi,",
"tha-ap353:7.4": "pasādassa idaṃ phalaṃ.",
"tha-ap353:8.1": "Paṭisambhidā catasso,",
"tha-ap353:8.2": "vimokkhāpi ca aṭṭhime;",
"tha-ap353:8.3": "Chaḷabhiññā sacchikatā,",
"tha-ap353:8.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap353:9.1": "Itthaṃ sudaṃ āyasmā saddasaññako thero imā gāthāyo abhāsitthāti.",
"tha-ap353:10.1": "Saddasaññakattherassāpadānaṃ paṭhamaṃ."
}