{
"tha-ap137:1.1": "Therāpadāna",
"tha-ap137:2.1": "Sobhitavagga",
"tha-ap137:3.1": "5 Rahosaññakattheraapadāna",
"tha-ap137:4.1": "“Himavantassāvidūre,",
"tha-ap137:4.2": "vasabho nāma pabbato;",
"tha-ap137:4.3": "Tasmiṃ pabbatapādamhi,",
"tha-ap137:4.4": "assamo āsi māpito.",
"tha-ap137:5.1": "Tīṇi sissasahassāni,",
"tha-ap137:5.2": "vācesiṃ brāhmaṇo tadā;",
"tha-ap137:5.3": "Saṃharitvāna te sisse,",
"tha-ap137:5.4": "ekamantaṃ upāvisiṃ.",
"tha-ap137:6.1": "Ekamantaṃ nisīditvā,",
"tha-ap137:6.2": "brāhmaṇo mantapāragū;",
"tha-ap137:6.3": "Buddhavedaṃ gavesanto,",
"tha-ap137:6.4": "ñāṇe cittaṃ pasādayiṃ.",
"tha-ap137:7.1": "Tattha cittaṃ pasādetvā,",
"tha-ap137:7.2": "nisīdiṃ paṇṇasanthare;",
"tha-ap137:7.3": "Pallaṅkaṃ ābhujitvāna,",
"tha-ap137:7.4": "tattha kālaṅkato ahaṃ.",
"tha-ap137:8.1": "Ekattiṃse ito kappe,",
"tha-ap137:8.2": "yaṃ saññamalabhiṃ tadā;",
"tha-ap137:8.3": "Duggatiṃ nābhijānāmi,",
"tha-ap137:8.4": "ñāṇasaññāyidaṃ phalaṃ.",
"tha-ap137:9.1": "Sattavīsatikappamhi,",
"tha-ap137:9.2": "rājā siridharo ahu;",
"tha-ap137:9.3": "Sattaratanasampanno,",
"tha-ap137:9.4": "cakkavattī mahabbalo.",
"tha-ap137:10.1": "Paṭisambhidā catasso, … pe …",
"tha-ap137:10.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap137:11.1": "Itthaṃ sudaṃ āyasmā rahosaññako thero imā gāthāyo abhāsitthāti.",
"tha-ap137:12.1": "Rahosaññakattherassāpadānaṃ pañcamaṃ."
}