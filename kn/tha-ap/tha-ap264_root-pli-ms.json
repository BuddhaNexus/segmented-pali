{
"tha-ap264:1.1": "Therāpadāna",
"tha-ap264:2.1": "Padumukkhipavagga",
"tha-ap264:3.1": "2. Telamakkhiyattheraapadāna",
"tha-ap264:4.1": "“Siddhatthamhi bhagavati,",
"tha-ap264:4.2": "nibbutamhi narāsabhe;",
"tha-ap264:4.3": "Bodhiyā vedikāyāhaṃ,",
"tha-ap264:4.4": "telaṃ makkhesi tāvade.",
"tha-ap264:5.1": "Catunnavutito kappe,",
"tha-ap264:5.2": "yaṃ telaṃ makkhayiṃ tadā;",
"tha-ap264:5.3": "Duggatiṃ nābhijānāmi,",
"tha-ap264:5.4": "makkhanāya idaṃ phalaṃ.",
"tha-ap264:6.1": "Catuvīse ito kappe,",
"tha-ap264:6.2": "succhavi nāma khattiyo;",
"tha-ap264:6.3": "Sattaratanasampanno,",
"tha-ap264:6.4": "cakkavattī mahabbalo.",
"tha-ap264:7.1": "Paṭisambhidā catasso, … pe …",
"tha-ap264:7.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap264:8.1": "Itthaṃ sudaṃ āyasmā telamakkhiyo thero imā gāthāyo abhāsitthāti.",
"tha-ap264:9.1": "Telamakkhiyattherassāpadānaṃ dutiyaṃ."
}