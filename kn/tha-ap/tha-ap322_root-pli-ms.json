{
"tha-ap322:1.1": "Therāpadāna",
"tha-ap322:2.1": "Ārakkhadāyakavagga",
"tha-ap322:3.1": "10. Kumudamāliyattheraapadāna",
"tha-ap322:4.1": "“Usabhaṃ pavaraṃ vīraṃ,",
"tha-ap322:4.2": "mahesiṃ vijitāvinaṃ;",
"tha-ap322:4.3": "Vipassinaṃ mahāvīraṃ,",
"tha-ap322:4.4": "abhijātaṃva kesariṃ.",
"tha-ap322:5.1": "Rathiyaṃ paṭipajjantaṃ,",
"tha-ap322:5.2": "āhutīnaṃ paṭiggahaṃ;",
"tha-ap322:5.3": "Gahetvā kumudaṃ mālaṃ,",
"tha-ap322:5.4": "buddhaseṭṭhaṃ samokiriṃ.",
"tha-ap322:6.1": "Ekanavutito kappe,",
"tha-ap322:6.2": "yaṃ pupphamabhipūjayiṃ;",
"tha-ap322:6.3": "Duggatiṃ nābhijānāmi,",
"tha-ap322:6.4": "buddhapūjāyidaṃ phalaṃ.",
"tha-ap322:7.1": "Paṭisambhidā catasso, … pe …",
"tha-ap322:7.4": "kataṃ buddhassa sāsanaṃ”. #2820",
"tha-ap322:8.1": "Itthaṃ sudaṃ āyasmā kumudamāliyo thero imā gāthāyo abhāsitthāti.",
"tha-ap322:9.1": "Kumudamāliyattherassāpadānaṃ dasamaṃ.",
"tha-ap322:10.1": "Ārakkhadāyakavaggo bāttiṃsatimo.",
"tha-ap322:11.1": "Tassuddānaṃ",
"tha-ap322:12.1": "Ārakkhado bhojanado,",
"tha-ap322:12.2": "gatasaññī padumiyo;",
"tha-ap322:12.3": "Pupphāsanī santhaviko,",
"tha-ap322:12.4": "saddasaññī tiraṃsiyo;",
"tha-ap322:12.5": "Kandaliko kumudī ca,",
"tha-ap322:12.6": "sattapaññāsa gāthakāti."
}