{
"tha-ap150:1.1": "Therāpadāna",
"tha-ap150:2.1": "Chattavagga",
"tha-ap150:3.1": "8. Phalakadāyakattheraapadāna",
"tha-ap150:4.1": "“Yānakāro pure āsiṃ,",
"tha-ap150:4.2": "dārukamme susikkhito;",
"tha-ap150:4.3": "Candanaṃ phalakaṃ katvā,",
"tha-ap150:4.4": "adāsiṃ lokabandhuno.",
"tha-ap150:5.1": "Pabhāsati idaṃ byamhaṃ,",
"tha-ap150:5.2": "suvaṇṇassa sunimmitaṃ;",
"tha-ap150:5.3": "Hatthiyānaṃ assayānaṃ,",
"tha-ap150:5.4": "dibbayānaṃ upaṭṭhitaṃ.",
"tha-ap150:6.1": "Pāsādā sivikā ceva,",
"tha-ap150:6.2": "Nibbattanti yadicchakaṃ;",
"tha-ap150:6.3": "Akkhobhaṃ ratanaṃ mayhaṃ,",
"tha-ap150:6.4": "Phalakassa idaṃ phalaṃ.",
"tha-ap150:7.1": "Ekanavutito kappe,",
"tha-ap150:7.2": "phalakaṃ yamahaṃ dadiṃ;",
"tha-ap150:7.3": "Duggatiṃ nābhijānāmi,",
"tha-ap150:7.4": "phalakassa idaṃ phalaṃ.",
"tha-ap150:8.1": "Sattapaññāsakappamhi,",
"tha-ap150:8.2": "caturo nimmitāvhayā;",
"tha-ap150:8.3": "Sattaratanasampannā,",
"tha-ap150:8.4": "cakkavattī mahabbalā.",
"tha-ap150:9.1": "Paṭisambhidā catasso, … pe …",
"tha-ap150:9.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap150:10.1": "Itthaṃ sudaṃ āyasmā phalakadāyako thero imā gāthāyo abhāsitthāti.",
"tha-ap150:11.1": "Phalakadāyakattherassāpadānaṃ aṭṭhamaṃ."
}