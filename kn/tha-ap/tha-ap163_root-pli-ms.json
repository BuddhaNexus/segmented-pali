{
"tha-ap163:1.1": "Therāpadāna",
"tha-ap163:2.1": "Supāricariyavagga",
"tha-ap163:3.1": "1. Supāricariyattheraapadāna",
"tha-ap163:4.1": "“Padumo nāma nāmena,",
"tha-ap163:4.2": "dvipadindo narāsabho;",
"tha-ap163:4.3": "Pavanā abhinikkhamma,",
"tha-ap163:4.4": "dhammaṃ deseti cakkhumā.",
"tha-ap163:5.1": "Yakkhānaṃ samayo āsi,",
"tha-ap163:5.2": "avidūre mahesino;",
"tha-ap163:5.3": "Yena kiccena sampattā,",
"tha-ap163:5.4": "ajjhāpekkhiṃsu tāvade.",
"tha-ap163:6.1": "Buddhassa giramaññāya,",
"tha-ap163:6.2": "amatassa ca desanaṃ;",
"tha-ap163:6.3": "Pasannacitto sumano,",
"tha-ap163:6.4": "apphoṭetvā upaṭṭhahiṃ.",
"tha-ap163:7.1": "Suciṇṇassa phalaṃ passa,",
"tha-ap163:7.2": "upaṭṭhānassa satthuno;",
"tha-ap163:7.3": "Tiṃsakappasahassesu,",
"tha-ap163:7.4": "duggatiṃ nupapajjahaṃ.",
"tha-ap163:8.1": "Ūnatiṃse kappasate,",
"tha-ap163:8.2": "samalaṅkatanāmako;",
"tha-ap163:8.3": "Sattaratanasampanno,",
"tha-ap163:8.4": "cakkavattī mahabbalo.",
"tha-ap163:9.1": "Paṭisambhidā catasso,",
"tha-ap163:9.2": "vimokkhāpi ca aṭṭhime;",
"tha-ap163:9.3": "Chaḷabhiññā sacchikatā,",
"tha-ap163:9.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap163:10.1": "Itthaṃ sudaṃ āyasmā supāricariyo thero imā gāthāyo abhāsitthāti.",
"tha-ap163:11.1": "Supāricariyattherassāpadānaṃ paṭhamaṃ."
}