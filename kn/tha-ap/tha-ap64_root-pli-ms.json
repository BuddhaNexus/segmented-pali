{
"tha-ap64:1.1": "Therāpadāna",
"tha-ap64:2.1": "Sakacintaniyavagga",
"tha-ap64:3.1": "2 Avopupphiyattheraapadāna",
"tha-ap64:4.1": "“Vihārā abhinikkhamma,",
"tha-ap64:4.2": "abbhuṭṭhāsi ca caṅkame;",
"tha-ap64:4.3": "Catusaccaṃ pakāsanto,",
"tha-ap64:4.4": "desento amataṃ padaṃ.",
"tha-ap64:5.1": "Sikhissa giramaññāya,",
"tha-ap64:5.2": "buddhaseṭṭhassa tādino;",
"tha-ap64:5.3": "Nānāpupphaṃ gahetvāna,",
"tha-ap64:5.4": "ākāsamhi samokiriṃ.",
"tha-ap64:6.1": "Tena kammena dvipadinda,",
"tha-ap64:6.2": "lokajeṭṭha narāsabha;",
"tha-ap64:6.3": "Pattomhi acalaṃ ṭhānaṃ,",
"tha-ap64:6.4": "hitvā jayaparājayaṃ.",
"tha-ap64:7.1": "Ekattiṃse ito kappe,",
"tha-ap64:7.2": "yaṃ pupphamabhiropayiṃ;",
"tha-ap64:7.3": "Duggatiṃ nābhijānāmi,",
"tha-ap64:7.4": "pupphapūjāyidaṃ phalaṃ.",
"tha-ap64:8.1": "Ito vīsatikappamhi,",
"tha-ap64:8.2": "sumedho nāma khattiyo;",
"tha-ap64:8.3": "Sattaratanasampanno,",
"tha-ap64:8.4": "cakkavattī mahabbalo.",
"tha-ap64:9.1": "Paṭisambhidā catasso, … pe …",
"tha-ap64:9.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap64:10.1": "Itthaṃ sudaṃ āyasmā avopupphiyo thero imā gāthāyo abhāsitthāti.",
"tha-ap64:11.1": "Avopupphiyattherassāpadānaṃ dutiyaṃ."
}