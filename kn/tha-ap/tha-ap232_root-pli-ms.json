{
"tha-ap232:1.1": "Therāpadāna",
"tha-ap232:2.1": "Ālambaṇadāyakavagga",
"tha-ap232:3.1": "10 Ekavandaniyattheraapadāna",
"tha-ap232:4.1": "“Usabhaṃ pavaraṃ vīraṃ,",
"tha-ap232:4.2": "vessabhuṃ vijitāvinaṃ;",
"tha-ap232:4.3": "Pasannacitto sumano,",
"tha-ap232:4.4": "buddhaseṭṭhamavandahaṃ.",
"tha-ap232:5.1": "Ekattiṃse ito kappe,",
"tha-ap232:5.2": "yaṃ kammamakariṃ tadā;",
"tha-ap232:5.3": "Duggatiṃ nābhijānāmi,",
"tha-ap232:5.4": "vandanāya idaṃ phalaṃ.",
"tha-ap232:6.1": "Catuvīsatikappamhi,",
"tha-ap232:6.2": "vikatānandanāmako;",
"tha-ap232:6.3": "Sattaratanasampanno,",
"tha-ap232:6.4": "cakkavattī mahabbalo.",
"tha-ap232:7.1": "Paṭisambhidā catasso, … pe …",
"tha-ap232:7.4": "kataṃ buddhassa sāsanaṃ”. #2419",
"tha-ap232:8.1": "Itthaṃ sudaṃ āyasmā ekavandaniyo thero imā gāthāyo abhāsitthāti.",
"tha-ap232:9.1": "Ekavandaniyattherassāpadānaṃ dasamaṃ.",
"tha-ap232:10.1": "Ālambaṇadāyakavaggo tevīsatimo.",
"tha-ap232:11.1": "Tassuddānaṃ",
"tha-ap232:12.1": "Ālambaṇañca ajinaṃ,",
"tha-ap232:12.2": "maṃsadārakkhadāyako;",
"tha-ap232:12.3": "Abyādhi aṅkolaṃ soṇṇaṃ,",
"tha-ap232:12.4": "miñjaāveḷavandanaṃ;",
"tha-ap232:12.5": "Pañcapaññāsa gāthāyo,",
"tha-ap232:12.6": "gaṇitā atthadassibhi."
}