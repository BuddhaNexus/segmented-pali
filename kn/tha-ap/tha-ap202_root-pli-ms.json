{
"tha-ap202:1.1": "Therāpadāna",
"tha-ap202:2.1": "Tamālapupphiyavagga",
"tha-ap202:3.1": "10 Yūthikapupphiyattheraapadāna",
"tha-ap202:4.1": "“Padumuttaro nāma jino,",
"tha-ap202:4.2": "āhutīnaṃ paṭiggaho;",
"tha-ap202:4.3": "Pavanā nikkhamitvāna,",
"tha-ap202:4.4": "vihāraṃ yāti cakkhumā.",
"tha-ap202:5.1": "Ubho hatthehi paggayha,",
"tha-ap202:5.2": "yūthikaṃ pupphamuttamaṃ;",
"tha-ap202:5.3": "Buddhassa abhiropayiṃ,",
"tha-ap202:5.4": "mettacittassa tādino.",
"tha-ap202:6.1": "Tena cittappasādena,",
"tha-ap202:6.2": "anubhotvāna sampadā;",
"tha-ap202:6.3": "Kappānaṃ satasahassaṃ,",
"tha-ap202:6.4": "duggatiṃ nupapajjahaṃ.",
"tha-ap202:7.1": "Ito paññāsakappesu,",
"tha-ap202:7.2": "eko āsiṃ janādhipo;",
"tha-ap202:7.3": "Samittanandano nāma,",
"tha-ap202:7.4": "cakkavattī mahabbalo.",
"tha-ap202:8.1": "Paṭisambhidā catasso, … pe …",
"tha-ap202:8.4": "kataṃ buddhassa sāsanaṃ”. #2265",
"tha-ap202:9.1": "Itthaṃ sudaṃ āyasmā yūthikapupphiyo thero imā gāthāyo abhāsitthāti.",
"tha-ap202:10.1": "Yūthikapupphiyattherassāpadānaṃ dasamaṃ.",
"tha-ap202:11.1": "Tamālapupphiyavaggo vīsatimo.",
"tha-ap202:12.1": "Tassuddānaṃ",
"tha-ap202:13.1": "Tamālatiṇasanthāro,",
"tha-ap202:13.2": "khaṇḍaphulli asokiyo;",
"tha-ap202:13.3": "Aṅkolakī kisalayo,",
"tha-ap202:13.4": "tinduko nelapupphiyo;",
"tha-ap202:13.5": "Kiṃkaṇiko yūthiko ca,",
"tha-ap202:13.6": "gāthā paññāsa aṭṭha cāti.",
"tha-ap202:14.1": "Atha vagguddānaṃ",
"tha-ap202:15.1": "Bhikkhādāyī parivāro,",
"tha-ap202:15.2": "sereyyo sobhito tathā;",
"tha-ap202:15.3": "Chattañca bandhujīvī ca,",
"tha-ap202:15.4": "supāricariyopi ca.",
"tha-ap202:16.1": "Kumudo kuṭajo ceva,",
"tha-ap202:16.2": "tamāli dasamo kato;",
"tha-ap202:16.3": "Chasatāni ca gāthāni,",
"tha-ap202:16.4": "chasaṭṭhi ca tatuttari.",
"tha-ap202:17.1": "Bhikkhāvaggadasakaṃ.",
"tha-ap202:18.1": "Dutiyasatakaṃ samattaṃ."
}