{
"tha-ap169:1.1": "Therāpadāna",
"tha-ap169:2.1": "Supāricariyavagga",
"tha-ap169:3.1": "7 Yūthikapupphiyattheraapadāna",
"tha-ap169:4.1": "“Candabhāgānadītīre,",
"tha-ap169:4.2": "anusotaṃ vajāmahaṃ;",
"tha-ap169:4.3": "Sayambhuṃ addasaṃ tattha,",
"tha-ap169:4.4": "sālarājaṃva phullitaṃ.",
"tha-ap169:5.1": "Pupphaṃ yūthikamādāya,",
"tha-ap169:5.2": "upagacchiṃ mahāmuniṃ;",
"tha-ap169:5.3": "Pasannacitto sumano,",
"tha-ap169:5.4": "buddhassa abhiropayiṃ.",
"tha-ap169:6.1": "Catunnavutito kappe,",
"tha-ap169:6.2": "yaṃ pupphamabhiropayiṃ;",
"tha-ap169:6.3": "Duggatiṃ nābhijānāmi,",
"tha-ap169:6.4": "buddhapūjāyidaṃ phalaṃ.",
"tha-ap169:7.1": "Sattasaṭṭhimhito kappe,",
"tha-ap169:7.2": "eko sāmuddharo ahuṃ;",
"tha-ap169:7.3": "Sattaratanasampanno,",
"tha-ap169:7.4": "cakkavattī mahabbalo.",
"tha-ap169:8.1": "Paṭisambhidā catasso, … pe …",
"tha-ap169:8.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap169:9.1": "Itthaṃ sudaṃ āyasmā yūthikapupphiyo thero imā gāthāyo abhāsitthāti.",
"tha-ap169:10.1": "Yūthikapupphiyattherassāpadānaṃ sattamaṃ."
}