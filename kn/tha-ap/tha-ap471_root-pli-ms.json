{
"tha-ap471:1.1": "Therāpadāna",
"tha-ap471:2.1": "Sālakusumiyavagga",
"tha-ap471:3.1": "9 Sayampaṭibhāniyattheraapadāna",
"tha-ap471:4.1": "“Kakudhaṃ vilasantaṃva,",
"tha-ap471:4.2": "devadevaṃ narāsabhaṃ;",
"tha-ap471:4.3": "Rathiyaṃ paṭipajjantaṃ,",
"tha-ap471:4.4": "ko disvā na pasīdati.",
"tha-ap471:5.1": "Tamandhakāraṃ nāsetvā,",
"tha-ap471:5.2": "santāretvā bahuṃ janaṃ;",
"tha-ap471:5.3": "Ñāṇālokena jotantaṃ,",
"tha-ap471:5.4": "ko disvā na pasīdati.",
"tha-ap471:6.1": "Vasīsatasahassehi,",
"tha-ap471:6.2": "nīyantaṃ lokanāyakaṃ;",
"tha-ap471:6.3": "Uddharantaṃ bahū satte,",
"tha-ap471:6.4": "ko disvā na pasīdati.",
"tha-ap471:7.1": "Āhanantaṃ dhammabheriṃ,",
"tha-ap471:7.2": "maddantaṃ titthiye gaṇe;",
"tha-ap471:7.3": "Sīhanādaṃ vinadantaṃ,",
"tha-ap471:7.4": "ko disvā na pasīdati.",
"tha-ap471:8.1": "Yāvatā brahmalokato,",
"tha-ap471:8.2": "āgantvāna sabrahmakā;",
"tha-ap471:8.3": "Pucchanti nipuṇe pañhe,",
"tha-ap471:8.4": "ko disvā na pasīdati.",
"tha-ap471:9.1": "Yassañjaliṃ karitvāna,",
"tha-ap471:9.2": "āyācanti sadevakā;",
"tha-ap471:9.3": "Tena puññaṃ anubhonti,",
"tha-ap471:9.4": "ko disvā na pasīdati.",
"tha-ap471:10.1": "Sabbe janā samāgantvā,",
"tha-ap471:10.2": "sampavārenti cakkhumaṃ;",
"tha-ap471:10.3": "Na vikampati ajjhiṭṭho,",
"tha-ap471:10.4": "ko disvā na pasīdati.",
"tha-ap471:11.1": "Nagaraṃ pavisato yassa,",
"tha-ap471:11.2": "ravanti bheriyo bahū;",
"tha-ap471:11.3": "Vinadanti gajā mattā,",
"tha-ap471:11.4": "ko disvā na pasīdati.",
"tha-ap471:12.1": "Vīthiyā gacchato yassa,",
"tha-ap471:12.2": "sabbābhā jotate sadā;",
"tha-ap471:12.3": "Abbhunnatā samā honti,",
"tha-ap471:12.4": "ko disvā na pasīdati.",
"tha-ap471:13.1": "Byāharantassa buddhassa,",
"tha-ap471:13.2": "cakkavāḷampi suyyati;",
"tha-ap471:13.3": "Sabbe satte viññāpeti,",
"tha-ap471:13.4": "ko disvā na pasīdati.",
"tha-ap471:14.1": "Satasahassito kappe,",
"tha-ap471:14.2": "yaṃ buddhamabhikittayiṃ;",
"tha-ap471:14.3": "Duggatiṃ nābhijānāmi,",
"tha-ap471:14.4": "kittanāya idaṃ phalaṃ.",
"tha-ap471:15.1": "Kilesā jhāpitā mayhaṃ, … pe …",
"tha-ap471:15.4": "viharāmi anāsavo.",
"tha-ap471:16.1": "Svāgataṃ vata me āsi, … pe …",
"tha-ap471:16.4": "kataṃ buddhassa sāsanaṃ.",
"tha-ap471:17.1": "Paṭisambhidā catasso, … pe …",
"tha-ap471:17.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap471:18.1": "Itthaṃ sudaṃ āyasmā sayampaṭibhāniyo thero imā gāthāyo abhāsitthāti.",
"tha-ap471:19.1": "Sayampaṭibhāniyattherassāpadānaṃ navamaṃ."
}