{
"tha-ap518:1.1": "Therāpadāna",
"tha-ap518:2.1": "Phaladāyakavagga",
"tha-ap518:3.1": "6. Ajjunapupphiyattheraapadāna",
"tha-ap518:4.1": "“Candabhāgānadītīre,",
"tha-ap518:4.2": "ahosiṃ kinnaro tadā;",
"tha-ap518:4.3": "Addasaṃ virajaṃ buddhaṃ,",
"tha-ap518:4.4": "sayambhuṃ aparājitaṃ.",
"tha-ap518:5.1": "Pasannacitto sumano,",
"tha-ap518:5.2": "vedajāto katañjalī;",
"tha-ap518:5.3": "Gahetvā ajjunaṃ pupphaṃ,",
"tha-ap518:5.4": "sayambhuṃ abhipūjayiṃ.",
"tha-ap518:6.1": "Tena kammena sukatena,",
"tha-ap518:6.2": "cetanāpaṇidhīhi ca;",
"tha-ap518:6.3": "Jahitvā kinnaraṃ dehaṃ,",
"tha-ap518:6.4": "tāvatiṃsamagacchahaṃ.",
"tha-ap518:7.1": "Chattiṃsakkhattuṃ devindo,",
"tha-ap518:7.2": "devarajjamakārayiṃ;",
"tha-ap518:7.3": "Dasakkhattuṃ cakkavattī,",
"tha-ap518:7.4": "mahārajjamakārayiṃ.",
"tha-ap518:8.1": "Padesarajjaṃ vipulaṃ,",
"tha-ap518:8.2": "gaṇanāto asaṅkhiyaṃ;",
"tha-ap518:8.3": "Sukhette vappitaṃ bījaṃ,",
"tha-ap518:8.4": "sayambhumhi aho mama.",
"tha-ap518:9.1": "Kusalaṃ vijjate mayhaṃ,",
"tha-ap518:9.2": "pabbajiṃ anagāriyaṃ;",
"tha-ap518:9.3": "Pūjāraho ahaṃ ajja,",
"tha-ap518:9.4": "sakyaputtassa sāsane.",
"tha-ap518:10.1": "Kilesā jhāpitā mayhaṃ, … pe …",
"tha-ap518:10.4": "viharāmi anāsavo.",
"tha-ap518:11.1": "Svāgataṃ vata me āsi, … pe …",
"tha-ap518:11.4": "kataṃ buddhassa sāsanaṃ.",
"tha-ap518:12.1": "Paṭisambhidā catasso, … pe …",
"tha-ap518:12.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap518:13.1": "Itthaṃ sudaṃ āyasmā ajjunapupphiyo thero imā gāthāyo abhāsitthāti.",
"tha-ap518:14.1": "Ajjunapupphiyattherassāpadānaṃ chaṭṭhaṃ."
}