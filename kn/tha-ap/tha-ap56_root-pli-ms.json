{
"tha-ap56:1.1": "Therāpadāna",
"tha-ap56:2.1": "Bījanivagga",
"tha-ap56:3.1": "4 Gandhodakiyattheraapadāna",
"tha-ap56:4.1": "“Padumuttarabuddhassa,",
"tha-ap56:4.2": "mahābodhimaho ahu;",
"tha-ap56:4.3": "Vicittaṃ ghaṭamādāya,",
"tha-ap56:4.4": "gandhodakamadāsahaṃ.",
"tha-ap56:5.1": "Nhānakāle ca bodhiyā,",
"tha-ap56:5.2": "mahāmegho pavassatha;",
"tha-ap56:5.3": "Ninnādo ca mahā āsi,",
"tha-ap56:5.4": "asaniyā phalantiyā.",
"tha-ap56:6.1": "Tenevāsanivegena,",
"tha-ap56:6.2": "tattha kālaṅkato ahaṃ;",
"tha-ap56:6.3": "Devaloke ṭhito santo,",
"tha-ap56:6.4": "imā gāthā abhāsahaṃ.",
"tha-ap56:7.1": "‘Aho buddhā aho dhammā,",
"tha-ap56:7.2": "aho no satthusampadā;",
"tha-ap56:7.3": "Kaḷevaraṃ me patitaṃ,",
"tha-ap56:7.4": "devaloke ramāmahaṃ.",
"tha-ap56:8.1": "Ubbiddhaṃ bhavanaṃ mayhaṃ,",
"tha-ap56:8.2": "Satabhūmaṃ samuggataṃ;",
"tha-ap56:8.3": "Kaññāsatasahassāni,",
"tha-ap56:8.4": "Parivārenti maṃ sadā.",
"tha-ap56:9.1": "Ābādhā me na vijjanti,",
"tha-ap56:9.2": "soko mayhaṃ na vijjati;",
"tha-ap56:9.3": "Pariḷāhaṃ na passāmi,",
"tha-ap56:9.4": "puññakammassidaṃ phalaṃ.",
"tha-ap56:10.1": "Aṭṭhavīse kappasate,",
"tha-ap56:10.2": "rājā saṃvasito ahuṃ;",
"tha-ap56:10.3": "Sattaratanasampanno,",
"tha-ap56:10.4": "cakkavattī mahabbalo’.",
"tha-ap56:11.1": "Paṭisambhidā catasso, … pe …",
"tha-ap56:11.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap56:12.1": "Itthaṃ sudaṃ āyasmā gandhodakiyo thero imā gāthāyo abhāsitthāti.",
"tha-ap56:13.1": "Gandhodakiyattherassāpadānaṃ catutthaṃ."
}