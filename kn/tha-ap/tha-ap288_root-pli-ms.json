{
"tha-ap288:1.1": "Therāpadāna",
"tha-ap288:2.1": "Paṇṇadāyakavagga",
"tha-ap288:3.1": "6 Upaṭṭhākadāyakattheraapadāna",
"tha-ap288:4.1": "“Rathiyaṃ paṭipajjantaṃ,",
"tha-ap288:4.2": "āhutīnaṃ paṭiggahaṃ;",
"tha-ap288:4.3": "Dvipadindaṃ mahānāgaṃ,",
"tha-ap288:4.4": "lokajeṭṭhaṃ narāsabhaṃ.",
"tha-ap288:5.1": "Pakkosāpiya tassāhaṃ,",
"tha-ap288:5.2": "sabbalokahitesino;",
"tha-ap288:5.3": "Upaṭṭhāko mayā dinno,",
"tha-ap288:5.4": "siddhatthassa mahesino.",
"tha-ap288:6.1": "Paṭiggahetvā sambuddho,",
"tha-ap288:6.2": "niyyādesi mahāmuni;",
"tha-ap288:6.3": "Uṭṭhāya āsanā tamhā,",
"tha-ap288:6.4": "pakkāmi pācināmukho.",
"tha-ap288:7.1": "Catunnavutito kappe,",
"tha-ap288:7.2": "upaṭṭhākamadaṃ tadā;",
"tha-ap288:7.3": "Duggatiṃ nābhijānāmi,",
"tha-ap288:7.4": "upaṭṭhānassidaṃ phalaṃ.",
"tha-ap288:8.1": "Sattapaññāsito kappe,",
"tha-ap288:8.2": "balasenasanāmako;",
"tha-ap288:8.3": "Sattaratanasampanno,",
"tha-ap288:8.4": "cakkavattī mahabbalo.",
"tha-ap288:9.1": "Paṭisambhidā catasso, … pe …",
"tha-ap288:9.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap288:10.1": "Itthaṃ sudaṃ āyasmā upaṭṭhākadāyako thero imā gāthāyo abhāsitthāti.",
"tha-ap288:11.1": "Upaṭṭhākadāyakattherassāpadānaṃ chaṭṭhaṃ."
}