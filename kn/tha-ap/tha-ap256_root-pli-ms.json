{
"tha-ap256:1.1": "Therāpadāna",
"tha-ap256:2.1": "Thomakavagga",
"tha-ap256:3.1": "4 Ticampakapupphiyattheraapadāna",
"tha-ap256:4.1": "“Himavantassāvidūre,",
"tha-ap256:4.2": "vikato nāma pabbato;",
"tha-ap256:4.3": "Tassa vemajjhe vasati,",
"tha-ap256:4.4": "samaṇo bhāvitindriyo.",
"tha-ap256:5.1": "Disvāna tassopasamaṃ,",
"tha-ap256:5.2": "vippasannena cetasā;",
"tha-ap256:5.3": "Tīṇi campakapupphāni,",
"tha-ap256:5.4": "gahetvāna samokiriṃ.",
"tha-ap256:6.1": "Ekanavutito kappe,",
"tha-ap256:6.2": "yaṃ pupphamabhipūjayiṃ;",
"tha-ap256:6.3": "Duggatiṃ nābhijānāmi,",
"tha-ap256:6.4": "buddhapūjāyidaṃ phalaṃ.",
"tha-ap256:7.1": "Paṭisambhidā catasso, … pe …",
"tha-ap256:7.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap256:8.1": "Itthaṃ sudaṃ āyasmā ticampakapupphiyo thero imā gāthāyo abhāsitthāti.",
"tha-ap256:9.1": "Ticampakapupphiyattherassāpadānaṃ catutthaṃ."
}