{
"tha-ap102:1.1": "Therāpadāna",
"tha-ap102:2.1": "Sudhāvagga",
"tha-ap102:3.1": "10. Buddhupaṭṭhākattheraapadāna",
"tha-ap102:4.1": "“Vipassissa bhagavato,",
"tha-ap102:4.2": "ahosiṃ saṅkhadhammako;",
"tha-ap102:4.3": "Niccupaṭṭhānayuttomhi,",
"tha-ap102:4.4": "sugatassa mahesino.",
"tha-ap102:5.1": "Upaṭṭhānaphalaṃ passa,",
"tha-ap102:5.2": "lokanāthassa tādino;",
"tha-ap102:5.3": "Saṭṭhitūriyasahassāni,",
"tha-ap102:5.4": "parivārenti maṃ sadā.",
"tha-ap102:6.1": "Ekanavutito kappe,",
"tha-ap102:6.2": "upaṭṭhahiṃ mahāisiṃ;",
"tha-ap102:6.3": "Duggatiṃ nābhijānāmi,",
"tha-ap102:6.4": "upaṭṭhānassidaṃ phalaṃ.",
"tha-ap102:7.1": "Catuvīse ito kappe,",
"tha-ap102:7.2": "mahānigghosanāmakā;",
"tha-ap102:7.3": "Soḷasāsiṃsu rājāno,",
"tha-ap102:7.4": "cakkavattī mahabbalā.",
"tha-ap102:8.1": "Paṭisambhidā catasso, … pe …",
"tha-ap102:8.4": "kataṃ buddhassa sāsanaṃ”. #1602",
"tha-ap102:9.1": "Itthaṃ sudaṃ āyasmā buddhupaṭṭhāko thero imā gāthāyo abhāsitthāti.",
"tha-ap102:10.1": "Buddhupaṭṭhākattherassāpadānaṃ dasamaṃ.",
"tha-ap102:11.1": "Sudhāvaggo dasamo.",
"tha-ap102:12.1": "Tassuddānaṃ",
"tha-ap102:13.1": "Sudhā sucinti ceḷañca,",
"tha-ap102:13.2": "sūcī ca gandhamāliyo;",
"tha-ap102:13.3": "Tipupphiyo madhusenā,",
"tha-ap102:13.4": "veyyāvacco cupaṭṭhako;",
"tha-ap102:13.5": "Samasaṭṭhi ca gāthāyo,",
"tha-ap102:13.6": "asmiṃ vagge pakittitā.",
"tha-ap102:14.1": "Atha vagguddānaṃ",
"tha-ap102:15.1": "Buddhavaggo hi paṭhamo,",
"tha-ap102:15.2": "sīhāsani subhūti ca;",
"tha-ap102:15.3": "Kuṇḍadhāno upāli ca,",
"tha-ap102:15.4": "bījanisakacinti ca.",
"tha-ap102:16.1": "Nāgasamālo timiro,",
"tha-ap102:16.2": "sudhāvaggena te dasa;",
"tha-ap102:16.3": "Catuddasasatā gāthā,",
"tha-ap102:16.4": "pañcapaññāsameva ca.",
"tha-ap102:17.1": "Buddhavaggadasakaṃ.",
"tha-ap102:18.1": "Paṭhamasatakaṃ samattaṃ."
}