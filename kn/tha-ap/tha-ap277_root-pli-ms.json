{
"tha-ap277:1.1": "Therāpadāna",
"tha-ap277:2.1": "Suvaṇṇabibbohanavagga",
"tha-ap277:3.1": "5 Ekañjalikattheraapadāna",
"tha-ap277:4.1": "“Udumbare vasantassa,",
"tha-ap277:4.2": "niyate paṇṇasanthare;",
"tha-ap277:4.3": "Vutthokāso mayā dinno,",
"tha-ap277:4.4": "samaṇassa mahesino.",
"tha-ap277:5.1": "Tissassa dvipadindassa,",
"tha-ap277:5.2": "lokanāthassa tādino;",
"tha-ap277:5.3": "Añjaliṃ paggahetvāna,",
"tha-ap277:5.4": "santhariṃ pupphasantharaṃ.",
"tha-ap277:6.1": "Dvenavute ito kappe,",
"tha-ap277:6.2": "yaṃ kariṃ pupphasantharaṃ;",
"tha-ap277:6.3": "Duggatiṃ nābhijānāmi,",
"tha-ap277:6.4": "santharassa idaṃ phalaṃ.",
"tha-ap277:7.1": "Ito cuddasakappamhi,",
"tha-ap277:7.2": "ahosiṃ manujādhipo;",
"tha-ap277:7.3": "Ekaañjaliko nāma,",
"tha-ap277:7.4": "cakkavattī mahabbalo.",
"tha-ap277:8.1": "Paṭisambhidā catasso, … pe …",
"tha-ap277:8.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap277:9.1": "Itthaṃ sudaṃ āyasmā ekañjaliko thero imā gāthāyo abhāsitthāti.",
"tha-ap277:10.1": "Ekañjalikattherassāpadānaṃ pañcamaṃ."
}