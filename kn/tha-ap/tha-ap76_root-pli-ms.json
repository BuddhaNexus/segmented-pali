{
"tha-ap76:1.1": "Therāpadāna",
"tha-ap76:2.1": "Nāgasamālavagga",
"tha-ap76:3.1": "4. Bhisāluvadāyakattheraapadāna",
"tha-ap76:4.1": "“Kānanaṃ vanamogayha,",
"tha-ap76:4.2": "vasāmi vipine ahaṃ;",
"tha-ap76:4.3": "Vipassiṃ addasaṃ buddhaṃ,",
"tha-ap76:4.4": "āhutīnaṃ paṭiggahaṃ.",
"tha-ap76:5.1": "Bhisāluvañca pādāsiṃ,",
"tha-ap76:5.2": "udakaṃ hatthadhovanaṃ;",
"tha-ap76:5.3": "Vanditvā sirasā pāde,",
"tha-ap76:5.4": "pakkāmi uttarāmukho.",
"tha-ap76:6.1": "Ekanavutito kappe,",
"tha-ap76:6.2": "bhisāluvamadaṃ tadā;",
"tha-ap76:6.3": "Duggatiṃ nābhijānāmi,",
"tha-ap76:6.4": "puññakammassidaṃ phalaṃ.",
"tha-ap76:7.1": "Ito tatiyake kappe,",
"tha-ap76:7.2": "bhisasammatakhattiyo;",
"tha-ap76:7.3": "Sattaratanasampanno,",
"tha-ap76:7.4": "cakkavattī mahabbalo.",
"tha-ap76:8.1": "Paṭisambhidā catasso, … pe …",
"tha-ap76:8.4": "kataṃ buddhassa sāsanaṃ”.",
"tha-ap76:9.1": "Itthaṃ sudaṃ āyasmā bhisāluvadāyako thero imā gāthāyo abhāsitthāti.",
"tha-ap76:10.1": "Bhisāluvadāyakattherassāpadānaṃ catutthaṃ.",
"tha-ap76:10.2": "Chaṭṭhabhāṇavāraṃ."
}