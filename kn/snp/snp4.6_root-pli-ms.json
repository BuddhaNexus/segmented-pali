{
"snp4.6:1.1": "Sutta Nipāta 4.6",
"snp4.6:2.1": "Jarāsutta",
"snp4.6:3.1": "Appaṃ vata jīvitaṃ idaṃ,",
"snp4.6:3.2": "Oraṃ vassasatāpi miyyati;",
"snp4.6:3.3": "Yo cepi aticca jīvati,",
"snp4.6:3.4": "Atha kho so jarasāpi miyyati.",
"snp4.6:4.1": "Socanti janā mamāyite, #2",
"snp4.6:4.2": "Na hi santi niccā pariggahā;",
"snp4.6:4.3": "Vinābhāvasantamevidaṃ,",
"snp4.6:4.4": "Iti disvā nāgāramāvase.",
"snp4.6:5.1": "Maraṇenapi taṃ pahīyati, #3",
"snp4.6:5.2": "Yaṃ puriso mamidanti maññati;",
"snp4.6:5.3": "Etampi viditvā paṇḍito,",
"snp4.6:5.4": "Na mamattāya nametha māmako.",
"snp4.6:6.1": "Supinena yathāpi saṅgataṃ, #4",
"snp4.6:6.2": "Paṭibuddho puriso na passati;",
"snp4.6:6.3": "Evampi piyāyitaṃ janaṃ,",
"snp4.6:6.4": "Petaṃ kālaṅkataṃ na passati.",
"snp4.6:7.1": "Diṭṭhāpi sutāpi te janā, #5",
"snp4.6:7.2": "Yesaṃ nāmamidaṃ pavuccati;",
"snp4.6:7.3": "Nāmaṃyevāvasissati,",
"snp4.6:7.4": "Akkheyyaṃ petassa jantuno.",
"snp4.6:8.1": "Sokapparidevamaccharaṃ, #6",
"snp4.6:8.2": "Na jahanti giddhā mamāyite;",
"snp4.6:8.3": "Tasmā munayo pariggahaṃ,",
"snp4.6:8.4": "Hitvā acariṃsu khemadassino.",
"snp4.6:9.1": "Patilīnacarassa bhikkhuno, #7",
"snp4.6:9.2": "Bhajamānassa vivittamāsanaṃ;",
"snp4.6:9.3": "Sāmaggiyamāhu tassa taṃ,",
"snp4.6:9.4": "Yo attānaṃ bhavane na dassaye.",
"snp4.6:10.1": "Sabbattha munī anissito, #8",
"snp4.6:10.2": "Na piyaṃ kubbati nopi appiyaṃ;",
"snp4.6:10.3": "Tasmiṃ paridevamaccharaṃ,",
"snp4.6:10.4": "Paṇṇe vāri yathā na limpati.",
"snp4.6:11.1": "Udabindu yathāpi pokkhare, #9",
"snp4.6:11.2": "Padume vāri yathā na limpati;",
"snp4.6:11.3": "Evaṃ muni nopalimpati,",
"snp4.6:11.4": "Yadidaṃ diṭṭhasutaṃ mutesu vā.",
"snp4.6:12.1": "Dhono na hi tena maññati, #10",
"snp4.6:12.2": "Yadidaṃ diṭṭhasutaṃ mutesu vā;",
"snp4.6:12.3": "Nāññena visuddhimicchati,",
"snp4.6:12.4": "Na hi so rajjati no virajjatīti.",
"snp4.6:13.1": "Jarāsuttaṃ chaṭṭhaṃ."
}