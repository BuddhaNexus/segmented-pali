{
"snp4.10:1.1": "Sutta Nipāta 4.10",
"snp4.10:2.1": "Purābhedasutta",
"snp4.10:3.1": "“Kathaṃdassī kathaṃsīlo,",
"snp4.10:3.2": "upasantoti vuccati;",
"snp4.10:3.3": "Taṃ me gotama pabrūhi,",
"snp4.10:3.4": "pucchito uttamaṃ naraṃ”.",
"snp4.10:4.1": "“Vītataṇho purā bhedā, #2",
"snp4.10:4.2": "iti bhagavā",
"snp4.10:4.3": "Pubbamantamanissito;",
"snp4.10:4.4": "Vemajjhe nupasaṅkheyyo,",
"snp4.10:4.5": "Tassa natthi purakkhataṃ.",
"snp4.10:5.1": "Akkodhano asantāsī, #3",
"snp4.10:5.2": "avikatthī akukkuco;",
"snp4.10:5.3": "Mantabhāṇī anuddhato,",
"snp4.10:5.4": "sa ve vācāyato muni.",
"snp4.10:6.1": "Nirāsatti anāgate, #4",
"snp4.10:6.2": "atītaṃ nānusocati;",
"snp4.10:6.3": "Vivekadassī phassesu,",
"snp4.10:6.4": "diṭṭhīsu ca na nīyati.",
"snp4.10:7.1": "Patilīno akuhako, #5",
"snp4.10:7.2": "apihālu amaccharī;",
"snp4.10:7.3": "Appagabbho ajeguccho,",
"snp4.10:7.4": "pesuṇeyye ca no yuto.",
"snp4.10:8.1": "Sātiyesu anassāvī, #6",
"snp4.10:8.2": "atimāne ca no yuto;",
"snp4.10:8.3": "Saṇho ca paṭibhānavā,",
"snp4.10:8.4": "na saddho na virajjati.",
"snp4.10:9.1": "Lābhakamyā na sikkhati, #7",
"snp4.10:9.2": "alābhe ca na kuppati;",
"snp4.10:9.3": "Aviruddho ca taṇhāya,",
"snp4.10:9.4": "rasesu nānugijjhati.",
"snp4.10:10.1": "Upekkhako sadā sato, #8",
"snp4.10:10.2": "na loke maññate samaṃ;",
"snp4.10:10.3": "Na visesī na nīceyyo,",
"snp4.10:10.4": "tassa no santi ussadā.",
"snp4.10:11.1": "Yassa nissayanā natthi, #9",
"snp4.10:11.2": "ñatvā dhammaṃ anissito;",
"snp4.10:11.3": "Bhavāya vibhavāya vā,",
"snp4.10:11.4": "taṇhā yassa na vijjati.",
"snp4.10:12.1": "Taṃ brūmi upasantoti, #10",
"snp4.10:12.2": "Kāmesu anapekkhinaṃ;",
"snp4.10:12.3": "Ganthā tassa na vijjanti,",
"snp4.10:12.4": "Atarī so visattikaṃ.",
"snp4.10:13.1": "Na tassa puttā pasavo, #11",
"snp4.10:13.2": "Khettaṃ vatthuñca vijjati;",
"snp4.10:13.3": "Attā vāpi nirattā vā,",
"snp4.10:13.4": "Na tasmiṃ upalabbhati.",
"snp4.10:14.1": "Yena naṃ vajjuṃ puthujjanā, #12",
"snp4.10:14.2": "Atho samaṇabrāhmaṇā;",
"snp4.10:14.3": "Taṃ tassa apurakkhataṃ,",
"snp4.10:14.4": "Tasmā vādesu nejati.",
"snp4.10:15.1": "Vītagedho amaccharī, #13",
"snp4.10:15.2": "Na ussesu vadate muni;",
"snp4.10:15.3": "Na samesu na omesu,",
"snp4.10:15.4": "Kappaṃ neti akappiyo.",
"snp4.10:16.1": "Yassa loke sakaṃ natthi, #14",
"snp4.10:16.2": "Asatā ca na socati;",
"snp4.10:16.3": "Dhammesu ca na gacchati,",
"snp4.10:16.4": "Sa ve santoti vuccatī”ti.",
"snp4.10:17.1": "Purābhedasuttaṃ dasamaṃ."
}