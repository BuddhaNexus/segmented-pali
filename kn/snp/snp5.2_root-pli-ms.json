{
"snp5.2:1.1": "Sutta Nipāta 5.2",
"snp5.2:2.1": "Ajitamāṇavapucchā",
"snp5.2:3.1": "“Kenassu nivuto loko,",
"snp5.2:3.2": "iccāyasmā ajito",
"snp5.2:3.3": "Kenassu nappakāsati;",
"snp5.2:3.4": "Kissābhilepanaṃ brūsi,",
"snp5.2:3.5": "Kiṃsu tassa mahabbhayaṃ”.",
"snp5.2:4.1": "“Avijjāya nivuto loko, #2",
"snp5.2:4.2": "ajitāti bhagavā",
"snp5.2:4.3": "Vevicchā pamādā nappakāsati;",
"snp5.2:4.4": "Jappābhilepanaṃ brūmi,",
"snp5.2:4.5": "Dukkhamassa mahabbhayaṃ”.",
"snp5.2:5.1": "“Savanti sabbadhi sotā, #3",
"snp5.2:5.2": "iccāyasmā ajito",
"snp5.2:5.3": "Sotānaṃ kiṃ nivāraṇaṃ;",
"snp5.2:5.4": "Sotānaṃ saṃvaraṃ brūhi,",
"snp5.2:5.5": "Kena sotā pidhiyyare”.",
"snp5.2:6.1": "“Yāni sotāni lokasmiṃ, #4",
"snp5.2:6.2": "ajitāti bhagavā",
"snp5.2:6.3": "Sati tesaṃ nivāraṇaṃ;",
"snp5.2:6.4": "Sotānaṃ saṃvaraṃ brūmi,",
"snp5.2:6.5": "Paññāyete pidhiyyare”.",
"snp5.2:7.1": "“Paññā ceva sati yañca, #5",
"snp5.2:7.2": "iccāyasmā ajito",
"snp5.2:7.3": "Nāmarūpañca mārisa;",
"snp5.2:7.4": "Etaṃ me puṭṭho pabrūhi,",
"snp5.2:7.5": "Katthetaṃ uparujjhati”.",
"snp5.2:8.1": "“Yametaṃ pañhaṃ apucchi, #6",
"snp5.2:8.2": "ajita taṃ vadāmi te;",
"snp5.2:8.3": "Yattha nāmañca rūpañca,",
"snp5.2:8.4": "asesaṃ uparujjhati;",
"snp5.2:8.5": "Viññāṇassa nirodhena,",
"snp5.2:8.6": "etthetaṃ uparujjhati”.",
"snp5.2:9.1": "“Ye ca saṅkhātadhammāse, #7",
"snp5.2:9.2": "ye ca sekhā puthū idha;",
"snp5.2:9.3": "Tesaṃ me nipako iriyaṃ,",
"snp5.2:9.4": "puṭṭho pabrūhi mārisa”.",
"snp5.2:10.1": "“Kāmesu nābhigijjheyya, #8",
"snp5.2:10.2": "Manasānāvilo siyā;",
"snp5.2:10.3": "Kusalo sabbadhammānaṃ,",
"snp5.2:10.4": "Sato bhikkhu paribbaje”ti.",
"snp5.2:11.1": "Ajitamāṇavapucchā paṭhamā."
}