{
"snp1.12:1.1": "Sutta Nipāta 1.12",
"snp1.12:2.1": "Munisutta",
"snp1.12:3.1": "Santhavāto bhayaṃ jātaṃ,",
"snp1.12:3.2": "niketā jāyate rajo;",
"snp1.12:3.3": "Aniketamasanthavaṃ,",
"snp1.12:3.4": "etaṃ ve munidassanaṃ.",
"snp1.12:4.1": "Yo jātamucchijja na ropayeyya, #2",
"snp1.12:4.2": "Jāyantamassa nānuppavecche;",
"snp1.12:4.3": "Tamāhu ekaṃ muninaṃ carantaṃ,",
"snp1.12:4.4": "Addakkhi so santipadaṃ mahesi.",
"snp1.12:5.1": "Saṅkhāya vatthūni pamāya bījaṃ, #3",
"snp1.12:5.2": "Sinehamassa nānuppavecche;",
"snp1.12:5.3": "Sa ve munī jātikhayantadassī,",
"snp1.12:5.4": "Takkaṃ pahāya na upeti saṅkhaṃ.",
"snp1.12:6.1": "Aññāya sabbāni nivesanāni, #4",
"snp1.12:6.2": "Anikāmayaṃ aññatarampi tesaṃ;",
"snp1.12:6.3": "Sa ve munī vītagedho agiddho,",
"snp1.12:6.4": "Nāyūhatī pāragato hi hoti.",
"snp1.12:7.1": "Sabbābhibhuṃ sabbaviduṃ sumedhaṃ, #5",
"snp1.12:7.2": "Sabbesu dhammesu anūpalittaṃ;",
"snp1.12:7.3": "Sabbañjahaṃ taṇhakkhaye vimuttaṃ,",
"snp1.12:7.4": "Taṃ vāpi dhīrā muni vedayanti.",
"snp1.12:8.1": "Paññābalaṃ sīlavatūpapannaṃ, #6",
"snp1.12:8.2": "Samāhitaṃ jhānarataṃ satīmaṃ;",
"snp1.12:8.3": "Saṅgā pamuttaṃ akhilaṃ anāsavaṃ,",
"snp1.12:8.4": "Taṃ vāpi dhīrā muni vedayanti.",
"snp1.12:9.1": "Ekaṃ carantaṃ munimappamattaṃ, #7",
"snp1.12:9.2": "Nindāpasaṃsāsu avedhamānaṃ;",
"snp1.12:9.3": "Sīhaṃva saddesu asantasantaṃ,",
"snp1.12:9.4": "Vātaṃva jālamhi asajjamānaṃ;",
"snp1.12:9.5": "Padmaṃva toyena alippamānaṃ,",
"snp1.12:9.6": "Netāramaññesamanaññaneyyaṃ;",
"snp1.12:9.7": "Taṃ vāpi dhīrā muni vedayanti.",
"snp1.12:10.1": "Yo ogahaṇe thambhorivābhijāyati, #8",
"snp1.12:10.2": "Yasmiṃ pare vācāpariyantaṃ vadanti;",
"snp1.12:10.3": "Taṃ vītarāgaṃ susamāhitindriyaṃ,",
"snp1.12:10.4": "Taṃ vāpi dhīrā muni vedayanti.",
"snp1.12:11.1": "Yo ve ṭhitatto tasaraṃva ujju, #9",
"snp1.12:11.2": "Jigucchati kammehi pāpakehi;",
"snp1.12:11.3": "Vīmaṃsamāno visamaṃ samañca,",
"snp1.12:11.4": "Taṃ vāpi dhīrā muni vedayanti.",
"snp1.12:12.1": "Yo saññatatto na karoti pāpaṃ, #10",
"snp1.12:12.2": "Daharo majjhimo ca muni yatatto;",
"snp1.12:12.3": "Arosaneyyo na so roseti kañci,",
"snp1.12:12.4": "Taṃ vāpi dhīrā muni vedayanti.",
"snp1.12:13.1": "Yadaggato majjhato sesato vā, #11",
"snp1.12:13.2": "Piṇḍaṃ labhetha paradattūpajīvī;",
"snp1.12:13.3": "Nālaṃ thutuṃ nopi nipaccavādī,",
"snp1.12:13.4": "Taṃ vāpi dhīrā muni vedayanti.",
"snp1.12:14.1": "Muniṃ carantaṃ virataṃ methunasmā, #12",
"snp1.12:14.2": "Yo yobbane nopanibajjhate kvaci;",
"snp1.12:14.3": "Madappamādā virataṃ vippamuttaṃ,",
"snp1.12:14.4": "Taṃ vāpi dhīrā muni vedayanti.",
"snp1.12:15.1": "Aññāya lokaṃ paramatthadassiṃ, #13",
"snp1.12:15.2": "Oghaṃ samuddaṃ atitariya tādiṃ;",
"snp1.12:15.3": "Taṃ chinnaganthaṃ asitaṃ anāsavaṃ,",
"snp1.12:15.4": "Taṃ vāpi dhīrā muni vedayanti.",
"snp1.12:16.1": "Asamā ubho dūravihāravuttino, #14",
"snp1.12:16.2": "Gihī dāraposī amamo ca subbato;",
"snp1.12:16.3": "Parapāṇarodhāya gihī asaññato,",
"snp1.12:16.4": "Niccaṃ munī rakkhati pāṇine yato.",
"snp1.12:17.1": "Sikhī yathā nīlagīvo vihaṅgamo, #15",
"snp1.12:17.2": "Haṃsassa nopeti javaṃ kudācanaṃ;",
"snp1.12:17.3": "Evaṃ gihī nānukaroti bhikkhuno,",
"snp1.12:17.4": "Munino vivittassa vanamhi jhāyatoti.",
"snp1.12:18.1": "Munisuttaṃ dvādasamaṃ.",
"snp1.12:19.1": "Uragavaggo paṭhamo.",
"snp1.12:20.1": "Urago dhaniyo ceva,",
"snp1.12:20.2": "visāṇañca tathā kasi;",
"snp1.12:20.3": "Cundo parābhavo ceva,",
"snp1.12:20.4": "vasalo mettabhāvanā.",
"snp1.12:21.1": "Sātāgiro āḷavako,",
"snp1.12:21.2": "Vijayo ca tathā muni;",
"snp1.12:21.3": "Dvādasetāni suttāni,",
"snp1.12:21.4": "Uragavaggoti vuccatīti."
}