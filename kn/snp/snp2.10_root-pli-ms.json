{
"snp2.10:1.1": "Sutta Nipāta 2.10",
"snp2.10:2.1": "Uṭṭhānasutta",
"snp2.10:3.1": "Uṭṭhahatha nisīdatha,",
"snp2.10:3.2": "ko attho supitena vo;",
"snp2.10:3.3": "Āturānañhi kā niddā,",
"snp2.10:3.4": "sallaviddhāna ruppataṃ.",
"snp2.10:4.1": "Uṭṭhahatha nisīdatha, #2",
"snp2.10:4.2": "Daḷhaṃ sikkhatha santiyā;",
"snp2.10:4.3": "Mā vo pamatte viññāya,",
"snp2.10:4.4": "Maccurājā amohayittha vasānuge.",
"snp2.10:5.1": "Yāya devā manussā ca, #3",
"snp2.10:5.2": "sitā tiṭṭhanti atthikā;",
"snp2.10:5.3": "Tarathetaṃ visattikaṃ,",
"snp2.10:5.4": "khaṇo vo mā upaccagā;",
"snp2.10:5.5": "Khaṇātītā hi socanti,",
"snp2.10:5.6": "nirayamhi samappitā.",
"snp2.10:6.1": "Pamādo rajo pamādo, #4",
"snp2.10:6.2": "Pamādānupatito rajo;",
"snp2.10:6.3": "Appamādena vijjāya,",
"snp2.10:6.4": "Abbahe sallamattanoti.",
"snp2.10:7.1": "Uṭṭhānasuttaṃ dasamaṃ."
}