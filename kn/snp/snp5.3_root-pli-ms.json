{
"snp5.3:1.1": "Sutta Nipāta 5.3",
"snp5.3:2.1": "Tissametteyyamāṇavapucchā",
"snp5.3:3.1": "“Kodha santusito loke,",
"snp5.3:3.2": "iccāyasmā tissametteyyo",
"snp5.3:3.3": "Kassa no santi iñjitā;",
"snp5.3:3.4": "Ko ubhantamabhiññāya,",
"snp5.3:3.5": "Majjhe mantā na lippati;",
"snp5.3:3.6": "Kaṃ brūsi mahāpurisoti,",
"snp5.3:3.7": "Ko idha sibbinimaccagā”.",
"snp5.3:4.1": "“Kāmesu brahmacariyavā, #2",
"snp5.3:4.2": "metteyyāti bhagavā",
"snp5.3:4.3": "Vītataṇho sadā sato;",
"snp5.3:4.4": "Saṅkhāya nibbuto bhikkhu,",
"snp5.3:4.5": "Tassa no santi iñjitā.",
"snp5.3:5.1": "So ubhantamabhiññāya, #3",
"snp5.3:5.2": "Majjhe mantā na lippati;",
"snp5.3:5.3": "Taṃ brūmi mahāpurisoti,",
"snp5.3:5.4": "So idha sibbinimaccagā”ti.",
"snp5.3:6.1": "Tissametteyyamāṇavapucchā dutiyā."
}