{
"snp5.11:1.1": "Sutta Nipāta 5.11",
"snp5.11:2.1": "Kappamāṇavapucchā",
"snp5.11:3.1": "“Majjhe sarasmiṃ tiṭṭhataṃ,",
"snp5.11:3.2": "iccāyasmā kappo",
"snp5.11:3.3": "Oghe jāte mahabbhaye;",
"snp5.11:3.4": "Jarāmaccuparetānaṃ,",
"snp5.11:3.5": "Dīpaṃ pabrūhi mārisa;",
"snp5.11:3.6": "Tvañca me dīpamakkhāhi,",
"snp5.11:3.7": "Yathāyidaṃ nāparaṃ siyā”.",
"snp5.11:4.1": "“Majjhe sarasmiṃ tiṭṭhataṃ, #2",
"snp5.11:4.2": "kappāti bhagavā",
"snp5.11:4.3": "Oghe jāte mahabbhaye;",
"snp5.11:4.4": "Jarāmaccuparetānaṃ,",
"snp5.11:4.5": "Dīpaṃ pabrūmi kappa te.",
"snp5.11:5.1": "Akiñcanaṃ anādānaṃ, #3",
"snp5.11:5.2": "etaṃ dīpaṃ anāparaṃ;",
"snp5.11:5.3": "Nibbānaṃ iti naṃ brūmi,",
"snp5.11:5.4": "jarāmaccuparikkhayaṃ.",
"snp5.11:6.1": "Etadaññāya ye satā, #4",
"snp5.11:6.2": "Diṭṭhadhammābhinibbutā;",
"snp5.11:6.3": "Na te māravasānugā,",
"snp5.11:6.4": "Na te mārassa paddhagū”ti.",
"snp5.11:7.1": "Kappamāṇavapucchā dasamā."
}