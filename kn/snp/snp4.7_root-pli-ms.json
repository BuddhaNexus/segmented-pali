{
"snp4.7:1.1": "Sutta Nipāta 4.7",
"snp4.7:2.1": "Tissametteyyasutta",
"snp4.7:3.1": "“Methunamanuyuttassa,",
"snp4.7:3.2": "iccāyasmā tisso metteyyo",
"snp4.7:3.3": "Vighātaṃ brūhi mārisa;",
"snp4.7:3.4": "Sutvāna tava sāsanaṃ,",
"snp4.7:3.5": "Viveke sikkhissāmase”.",
"snp4.7:4.1": "“Methunamanuyuttassa, #2",
"snp4.7:4.2": "metteyyāti bhagavā",
"snp4.7:4.3": "Mussate vāpi sāsanaṃ;",
"snp4.7:4.4": "Micchā ca paṭipajjati,",
"snp4.7:4.5": "Etaṃ tasmiṃ anāriyaṃ.",
"snp4.7:5.1": "Eko pubbe caritvāna, #3",
"snp4.7:5.2": "methunaṃ yo nisevati;",
"snp4.7:5.3": "Yānaṃ bhantaṃva taṃ loke,",
"snp4.7:5.4": "hīnamāhu puthujjanaṃ.",
"snp4.7:6.1": "Yaso kitti ca yā pubbe, #4",
"snp4.7:6.2": "hāyate vāpi tassa sā;",
"snp4.7:6.3": "Etampi disvā sikkhetha,",
"snp4.7:6.4": "methunaṃ vippahātave.",
"snp4.7:7.1": "Saṅkappehi pareto so, #5",
"snp4.7:7.2": "kapaṇo viya jhāyati;",
"snp4.7:7.3": "Sutvā paresaṃ nigghosaṃ,",
"snp4.7:7.4": "maṅku hoti tathāvidho.",
"snp4.7:8.1": "Atha satthāni kurute, #6",
"snp4.7:8.2": "paravādehi codito;",
"snp4.7:8.3": "Esa khvassa mahāgedho,",
"snp4.7:8.4": "mosavajjaṃ pagāhati.",
"snp4.7:9.1": "Paṇḍitoti samaññāto, #7",
"snp4.7:9.2": "ekacariyaṃ adhiṭṭhito;",
"snp4.7:9.3": "Athāpi methune yutto,",
"snp4.7:9.4": "mandova parikissati.",
"snp4.7:10.1": "Etamādīnavaṃ ñatvā, #8",
"snp4.7:10.2": "Muni pubbāpare idha;",
"snp4.7:10.3": "Ekacariyaṃ daḷhaṃ kayirā,",
"snp4.7:10.4": "Na nisevetha methunaṃ.",
"snp4.7:11.1": "Vivekaññeva sikkhetha, #9",
"snp4.7:11.2": "etaṃ ariyānamuttamaṃ;",
"snp4.7:11.3": "Na tena seṭṭho maññetha,",
"snp4.7:11.4": "sa ve nibbānasantike.",
"snp4.7:12.1": "Rittassa munino carato, #10",
"snp4.7:12.2": "Kāmesu anapekkhino;",
"snp4.7:12.3": "Oghatiṇṇassa pihayanti,",
"snp4.7:12.4": "Kāmesu gadhitā pajā”ti.",
"snp4.7:13.1": "Tissametteyyasuttaṃ sattamaṃ."
}