{
"snp5.16:1.1": "Sutta Nipāta 5.16",
"snp5.16:2.1": "Mogharājamāṇavapucchā",
"snp5.16:3.1": "“Dvāhaṃ sakkaṃ apucchissaṃ,",
"snp5.16:3.2": "iccāyasmā mogharājā",
"snp5.16:3.3": "Na me byākāsi cakkhumā;",
"snp5.16:3.4": "Yāvatatiyañca devīsi,",
"snp5.16:3.5": "Byākarotīti me sutaṃ.",
"snp5.16:4.1": "Ayaṃ loko paro loko, #2",
"snp5.16:4.2": "brahmaloko sadevako;",
"snp5.16:4.3": "Diṭṭhiṃ te nābhijānāti,",
"snp5.16:4.4": "gotamassa yasassino.",
"snp5.16:5.1": "Evaṃ abhikkantadassāviṃ, #3",
"snp5.16:5.2": "atthi pañhena āgamaṃ;",
"snp5.16:5.3": "Kathaṃ lokaṃ avekkhantaṃ,",
"snp5.16:5.4": "maccurājā na passati”.",
"snp5.16:6.1": "“Suññato lokaṃ avekkhassu, #4",
"snp5.16:6.2": "Mogharāja sadā sato;",
"snp5.16:6.3": "Attānudiṭṭhiṃ ūhacca,",
"snp5.16:6.4": "Evaṃ maccutaro siyā;",
"snp5.16:6.5": "Evaṃ lokaṃ avekkhantaṃ,",
"snp5.16:6.6": "Maccurājā na passatī”ti.",
"snp5.16:7.1": "Mogharājamāṇavapucchā pannarasamā."
}