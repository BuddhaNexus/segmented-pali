{
"thi-ap5:1.1": "Therīapadāna",
"thi-ap5:2.1": "Sumedhāvagga",
"thi-ap5:3.1": "5. Naḷamālikātherīapadāna",
"thi-ap5:4.1": "“Candabhāgānadītīre,",
"thi-ap5:4.2": "ahosiṃ kinnarī tadā;",
"thi-ap5:4.3": "Addasaṃ virajaṃ buddhaṃ,",
"thi-ap5:4.4": "sayambhuṃ aparājitaṃ.",
"thi-ap5:5.1": "Pasannacittā sumanā,",
"thi-ap5:5.2": "vedajātā katañjalī;",
"thi-ap5:5.3": "Naḷamālaṃ gahetvāna,",
"thi-ap5:5.4": "sayambhuṃ abhipūjayiṃ.",
"thi-ap5:6.1": "Tena kammena sukatena,",
"thi-ap5:6.2": "cetanāpaṇidhīhi ca;",
"thi-ap5:6.3": "Jahitvā kinnarīdehaṃ,",
"thi-ap5:6.4": "agacchiṃ tidasaṅgatiṃ.",
"thi-ap5:7.1": "Chattiṃsadevarājūnaṃ,",
"thi-ap5:7.2": "mahesittamakārayiṃ;",
"thi-ap5:7.3": "Dasannaṃ cakkavattīnaṃ,",
"thi-ap5:7.4": "mahesittamakārayiṃ;",
"thi-ap5:7.5": "Saṃvejetvāna me cittaṃ,",
"thi-ap5:7.6": "pabbajiṃ anagāriyaṃ.",
"thi-ap5:8.1": "Kilesā jhāpitā mayhaṃ,",
"thi-ap5:8.2": "bhavā sabbe samūhatā;",
"thi-ap5:8.3": "Sabbāsavā parikkhīṇā,",
"thi-ap5:8.4": "natthi dāni punabbhavo.",
"thi-ap5:9.1": "Catunnavutito kappe,",
"thi-ap5:9.2": "yaṃ pupphamabhipūjayiṃ;",
"thi-ap5:9.3": "Duggatiṃ nābhijānāmi,",
"thi-ap5:9.4": "pupphapūjāyidaṃ phalaṃ.",
"thi-ap5:10.1": "Kilesā jhāpitā mayhaṃ, … pe …",
"thi-ap5:10.4": "viharāmi anāsavā.",
"thi-ap5:11.1": "Svāgataṃ vata me āsi, … pe …",
"thi-ap5:11.4": "kataṃ buddhassa sāsanaṃ.",
"thi-ap5:12.1": "Paṭisambhidā catasso, … pe …",
"thi-ap5:12.4": "kataṃ buddhassa sāsanaṃ”.",
"thi-ap5:13.1": "Itthaṃ sudaṃ naḷamālikā therī imā gāthāyo abhāsitthāti.",
"thi-ap5:14.1": "Naḷamālikātheriyāpadānaṃ pañcamaṃ."
}