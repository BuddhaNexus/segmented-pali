{
"thi-ap19:1.1": "Therīapadāna",
"thi-ap19:2.1": "Ekūposathikavagga",
"thi-ap19:3.1": "9. Uppalavaṇṇātherīapadāna",
"thi-ap19:4.1": "“Bhikkhunī uppalavaṇṇā,",
"thi-ap19:4.2": "iddhiyā pāramiṃ gatā;",
"thi-ap19:4.3": "Vanditvā satthuno pāde,",
"thi-ap19:4.4": "idaṃ vacanamabravi.",
"thi-ap19:5.1": "‘Nitthiṇṇā jātisaṃsāraṃ,",
"thi-ap19:5.2": "pattāhaṃ acalaṃ padaṃ;",
"thi-ap19:5.3": "Sabbadukkhaṃ mayā khīṇaṃ,",
"thi-ap19:5.4": "ārocemi mahāmuni.",
"thi-ap19:6.1": "Yāvatā parisā atthi,",
"thi-ap19:6.2": "pasannā jinasāsane;",
"thi-ap19:6.3": "Yassā ca meparādhotthi,",
"thi-ap19:6.4": "khamantu jinasammukhā.",
"thi-ap19:7.1": "Saṃsāre saṃsarantiyā,",
"thi-ap19:7.2": "khalitaṃ me sace bhave;",
"thi-ap19:7.3": "Ārocemi mahāvīra,",
"thi-ap19:7.4": "aparādhaṃ khamassu taṃ’.",
"thi-ap19:8.1": "‘Iddhiñcāpi nidassehi,",
"thi-ap19:8.2": "mama sāsanakārike;",
"thi-ap19:8.3": "Catasso parisā ajja,",
"thi-ap19:8.4": "kaṅkhaṃ chindāhi yāvatā’.",
"thi-ap19:9.1": "‘Dhītā tuyhaṃ mahāvīra,",
"thi-ap19:9.2": "paññavanta jutindhara;",
"thi-ap19:9.3": "Bahuñca dukkaraṃ kammaṃ,",
"thi-ap19:9.4": "kataṃ me atidukkaraṃ.",
"thi-ap19:10.1": "Uppalasseva me vaṇṇo,",
"thi-ap19:10.2": "nāmenuppalanāmikā;",
"thi-ap19:10.3": "Sāvikā te mahāvīra,",
"thi-ap19:10.4": "pāde vandāmi cakkhuma.",
"thi-ap19:11.1": "Rāhulo ca ahañceva,",
"thi-ap19:11.2": "nekajātisate bahū;",
"thi-ap19:11.3": "Ekasmiṃ sambhave jātā,",
"thi-ap19:11.4": "samānachandamānasā.",
"thi-ap19:12.1": "Nibbatti ekato hoti,",
"thi-ap19:12.2": "jātiyāpi ca ekato;",
"thi-ap19:12.3": "Pacchime bhave sampatte,",
"thi-ap19:12.4": "ubhopi nānāsambhavā.",
"thi-ap19:13.1": "Putto ca rāhulo nāma,",
"thi-ap19:13.2": "dhītā uppalasavhayā;",
"thi-ap19:13.3": "Passa vīra mamaṃ iddhiṃ,",
"thi-ap19:13.4": "balaṃ dassemi satthuno’.",
"thi-ap19:14.1": "Mahāsamudde caturo,",
"thi-ap19:14.2": "pakkhipi hatthapātiyaṃ;",
"thi-ap19:14.3": "Telaṃ hatthagatañceva,",
"thi-ap19:14.4": "khiḍḍo komārako yathā.",
"thi-ap19:15.1": "Ubbattayitvā pathaviṃ,",
"thi-ap19:15.2": "pakkhipi hatthapātiyaṃ;",
"thi-ap19:15.3": "Cittaṃ muñjaṃ yathā nāma,",
"thi-ap19:15.4": "luñci komārako yuvā.",
"thi-ap19:16.1": "Cakkavāḷasamaṃ pāṇiṃ,",
"thi-ap19:16.2": "chādayitvāna matthake;",
"thi-ap19:16.3": "Vassāpetvāna phusitaṃ,",
"thi-ap19:16.4": "nānāvaṇṇaṃ punappunaṃ.",
"thi-ap19:17.1": "Bhūmiṃ udukkhalaṃ katvā,",
"thi-ap19:17.2": "dhaññaṃ katvāna sakkharaṃ;",
"thi-ap19:17.3": "Sineruṃ musalaṃ katvā,",
"thi-ap19:17.4": "maddi komārikā yathā.",
"thi-ap19:18.1": "‘Dhītāhaṃ buddhaseṭṭhassa,",
"thi-ap19:18.2": "nāmenuppalasavhayā;",
"thi-ap19:18.3": "Abhiññāsu vasībhūtā,",
"thi-ap19:18.4": "tava sāsanakārikā.",
"thi-ap19:19.1": "Nānāvikubbanaṃ katvā,",
"thi-ap19:19.2": "dassetvā lokanāyakaṃ;",
"thi-ap19:19.3": "Nāmagottañca sāvetvā,",
"thi-ap19:19.4": "pāde vandāmi cakkhuma.",
"thi-ap19:20.1": "Iddhīsu ca vasī homi,",
"thi-ap19:20.2": "dibbāya sotadhātuyā;",
"thi-ap19:20.3": "Cetopariyañāṇassa,",
"thi-ap19:20.4": "vasī homi mahāmune.",
"thi-ap19:21.1": "Pubbenivāsaṃ jānāmi,",
"thi-ap19:21.2": "dibbacakkhu visodhitaṃ;",
"thi-ap19:21.3": "Sabbāsavā parikkhīṇā,",
"thi-ap19:21.4": "natthi dāni punabbhavo.",
"thi-ap19:22.1": "Atthadhammaniruttīsu,",
"thi-ap19:22.2": "Paṭibhāne tatheva ca;",
"thi-ap19:22.3": "Ñāṇaṃ me vimalaṃ suddhaṃ,",
"thi-ap19:22.4": "Sabhāvena mahesino.",
"thi-ap19:23.1": "Purimānaṃ jinaggānaṃ,",
"thi-ap19:23.2": "saṅgamaṃ te nidassitaṃ;",
"thi-ap19:23.3": "Adhikāraṃ bahuṃ mayhaṃ,",
"thi-ap19:23.4": "tuyhatthāya mahāmuni.",
"thi-ap19:24.1": "Yaṃ mayā pūritaṃ kammaṃ,",
"thi-ap19:24.2": "Kusalaṃ sara me muni;",
"thi-ap19:24.3": "Tavatthāya mahāvīra,",
"thi-ap19:24.4": "Puññaṃ upacitaṃ mayā.",
"thi-ap19:25.1": "Abhabbaṭṭhāne vajjetvā,",
"thi-ap19:25.2": "vārayantī anācaraṃ;",
"thi-ap19:25.3": "Tavatthāya mahāvīra,",
"thi-ap19:25.4": "cattaṃ me jīvituttamaṃ.",
"thi-ap19:26.1": "Dasakoṭisahassāni,",
"thi-ap19:26.2": "adāsiṃ mama jīvitaṃ;",
"thi-ap19:26.3": "Pariccattā ca me homi,",
"thi-ap19:26.4": "tavatthāya mahāmuni’.",
"thi-ap19:27.1": "Tadātivimhitā sabbā,",
"thi-ap19:27.2": "sirasāva katañjalī;",
"thi-ap19:27.3": "‘Avocayye kathaṃ āsi,",
"thi-ap19:27.4": "atuliddhiparakkamā’.",
"thi-ap19:28.1": "Satasahassito kappe,",
"thi-ap19:28.2": "nāgakaññā ahaṃ tadā;",
"thi-ap19:28.3": "Vimalā nāma nāmena,",
"thi-ap19:28.4": "kaññānaṃ sādhusammatā.",
"thi-ap19:29.1": "Mahorago mahānāgo,",
"thi-ap19:29.2": "pasanno jinasāsane;",
"thi-ap19:29.3": "Padumuttaraṃ mahātejaṃ,",
"thi-ap19:29.4": "nimantesi sasāvakaṃ.",
"thi-ap19:30.1": "Ratanamayaṃ maṇḍapaṃ,",
"thi-ap19:30.2": "pallaṅkaṃ ratanāmayaṃ;",
"thi-ap19:30.3": "Ratanaṃ vālukākiṇṇaṃ,",
"thi-ap19:30.4": "upabhogaṃ ratanāmayaṃ.",
"thi-ap19:31.1": "Maggañca paṭiyādesi,",
"thi-ap19:31.2": "ratanaddhajabhūsitaṃ;",
"thi-ap19:31.3": "Paccuggantvāna sambuddhaṃ,",
"thi-ap19:31.4": "vajjanto tūriyehi so.",
"thi-ap19:32.1": "Parisāhi ca catūhi,",
"thi-ap19:32.2": "parivuto lokanāyako;",
"thi-ap19:32.3": "Mahoragassa bhavane,",
"thi-ap19:32.4": "nisīdi paramāsane.",
"thi-ap19:33.1": "Annaṃ pānaṃ khādanīyaṃ,",
"thi-ap19:33.2": "bhojanañca mahārahaṃ;",
"thi-ap19:33.3": "Varaṃ varañca pādāsi,",
"thi-ap19:33.4": "nāgarājā mahāyasaṃ.",
"thi-ap19:34.1": "Bhuñjitvāna sambuddho,",
"thi-ap19:34.2": "pattaṃ dhovitvā yoniso;",
"thi-ap19:34.3": "Anumodanīyaṃkāsi,",
"thi-ap19:34.4": "nāgakaññā mahiddhikā.",
"thi-ap19:35.1": "Sabbaññuṃ phullitaṃ disvā,",
"thi-ap19:35.2": "nāgakaññā mahāyasaṃ;",
"thi-ap19:35.3": "Pasannaṃ satthuno cittaṃ,",
"thi-ap19:35.4": "sunibandhañca mānasaṃ.",
"thi-ap19:36.1": "Mamañca cittamaññāya,",
"thi-ap19:36.2": "jalajuttamanāmako;",
"thi-ap19:36.3": "Tasmiṃ khaṇe mahāvīro,",
"thi-ap19:36.4": "bhikkhuniṃ dassayiddhiyā.",
"thi-ap19:37.1": "Iddhī anekā dassesi,",
"thi-ap19:37.2": "bhikkhunī sā visāradā;",
"thi-ap19:37.3": "Pamoditā vedajātā,",
"thi-ap19:37.4": "satthāraṃ idamabravi.",
"thi-ap19:38.1": "‘Addasāhaṃ imaṃ iddhiṃ,",
"thi-ap19:38.2": "sumanaṃ itarāyapi;",
"thi-ap19:38.3": "Kathaṃ ahosi sā vīra,",
"thi-ap19:38.4": "iddhiyā suvisāradā’.",
"thi-ap19:39.1": "‘Orasā mukhato jātā,",
"thi-ap19:39.2": "dhītā mama mahiddhikā;",
"thi-ap19:39.3": "Mamānusāsanikarā,",
"thi-ap19:39.4": "iddhiyā suvisāradā’.",
"thi-ap19:40.1": "Buddhassa vacanaṃ sutvā,",
"thi-ap19:40.2": "evaṃ patthesahaṃ tadā;",
"thi-ap19:40.3": "‘Ahampi tādisā homi,",
"thi-ap19:40.4": "iddhiyā suvisāradā.",
"thi-ap19:41.1": "Pamoditāhaṃ sumanā,",
"thi-ap19:41.2": "patthe uttamamānasā;",
"thi-ap19:41.3": "Anāgatamhi addhāne,",
"thi-ap19:41.4": "īdisā homi nāyaka’.",
"thi-ap19:42.1": "Maṇimayamhi pallaṅke,",
"thi-ap19:42.2": "maṇḍapamhi pabhassare;",
"thi-ap19:42.3": "Annapānena tappetvā,",
"thi-ap19:42.4": "sasaṃghaṃ lokanāyakaṃ.",
"thi-ap19:43.1": "Nāgānaṃ pavaraṃ pupphaṃ,",
"thi-ap19:43.2": "aruṇaṃ nāma uppalaṃ;",
"thi-ap19:43.3": "‘Vaṇṇaṃ me īdisaṃ hotu’,",
"thi-ap19:43.4": "pūjesiṃ lokanāyakaṃ.",
"thi-ap19:44.1": "Tena kammena sukatena,",
"thi-ap19:44.2": "cetanāpaṇidhīhi ca;",
"thi-ap19:44.3": "Jahitvā mānusaṃ dehaṃ,",
"thi-ap19:44.4": "tāvatiṃsamagacchahaṃ.",
"thi-ap19:45.1": "Tato cutāhaṃ manuje,",
"thi-ap19:45.2": "upapannā sayambhuno;",
"thi-ap19:45.3": "Uppalehi paṭicchannaṃ,",
"thi-ap19:45.4": "piṇḍapātamadāsahaṃ.",
"thi-ap19:46.1": "Ekanavutito kappe,",
"thi-ap19:46.2": "vipassī nāma nāyako;",
"thi-ap19:46.3": "Uppajji cārudassano,",
"thi-ap19:46.4": "sabbadhammesu cakkhumā.",
"thi-ap19:47.1": "Seṭṭhidhītā tadā hutvā,",
"thi-ap19:47.2": "bārāṇasipuruttame;",
"thi-ap19:47.3": "Nimantetvāna sambuddhaṃ,",
"thi-ap19:47.4": "sasaṃghaṃ lokanāyakaṃ.",
"thi-ap19:48.1": "Mahādānaṃ daditvāna,",
"thi-ap19:48.2": "uppalehi vināyakaṃ;",
"thi-ap19:48.3": "Pūjayitvā cetasāva,",
"thi-ap19:48.4": "vaṇṇasobhaṃ apatthayiṃ.",
"thi-ap19:49.1": "Imamhi bhaddake kappe,",
"thi-ap19:49.2": "brahmabandhu mahāyaso;",
"thi-ap19:49.3": "Kassapo nāma gottena,",
"thi-ap19:49.4": "uppajji vadataṃ varo.",
"thi-ap19:50.1": "Upaṭṭhāko mahesissa,",
"thi-ap19:50.2": "tadā āsi narissaro;",
"thi-ap19:50.3": "Kāsirājā kikī nāma,",
"thi-ap19:50.4": "bārāṇasipuruttame.",
"thi-ap19:51.1": "Tassāsiṃ dutiyā dhītā,",
"thi-ap19:51.2": "samaṇaguttasavhayā;",
"thi-ap19:51.3": "Dhammaṃ sutvā jinaggassa,",
"thi-ap19:51.4": "pabbajjaṃ samarocayiṃ.",
"thi-ap19:52.1": "Anujāni na no tāto,",
"thi-ap19:52.2": "agāreva tadā mayaṃ;",
"thi-ap19:52.3": "Vīsavassasahassāni,",
"thi-ap19:52.4": "vicarimha atanditā.",
"thi-ap19:53.1": "Komāribrahmacariyaṃ,",
"thi-ap19:53.2": "rājakaññā sukhedhitā;",
"thi-ap19:53.3": "Buddhopaṭṭhānaniratā,",
"thi-ap19:53.4": "muditā sattadhītaro.",
"thi-ap19:54.1": "Samaṇī samaṇaguttā ca,",
"thi-ap19:54.2": "bhikkhunī bhikkhudāyikā;",
"thi-ap19:54.3": "Dhammā ceva sudhammā ca,",
"thi-ap19:54.4": "sattamī saṃghadāyikā.",
"thi-ap19:55.1": "Ahaṃ khemā ca sappaññā,",
"thi-ap19:55.2": "paṭācārā ca kuṇḍalā;",
"thi-ap19:55.3": "Kisāgotamī dhammadinnā,",
"thi-ap19:55.4": "visākhā hoti sattamī.",
"thi-ap19:56.1": "Tehi kammehi sukatehi,",
"thi-ap19:56.2": "cetanāpaṇidhīhi ca;",
"thi-ap19:56.3": "Jahitvā mānusaṃ dehaṃ,",
"thi-ap19:56.4": "tāvatiṃsamagacchahaṃ.",
"thi-ap19:57.1": "Tato cutā manussesu,",
"thi-ap19:57.2": "upapannā mahākule;",
"thi-ap19:57.3": "Pītaṃ maṭṭhaṃ varaṃ dussaṃ,",
"thi-ap19:57.4": "adaṃ arahato ahaṃ.",
"thi-ap19:58.1": "Tato cutāriṭṭhapure,",
"thi-ap19:58.2": "jātā vippakule ahaṃ;",
"thi-ap19:58.3": "Dhītā tiriṭivacchassa,",
"thi-ap19:58.4": "ummādantī manoharā.",
"thi-ap19:59.1": "Tato cutā janapade,",
"thi-ap19:59.2": "kule aññatare ahaṃ;",
"thi-ap19:59.3": "Pasūtā nātiphītamhi,",
"thi-ap19:59.4": "sāliṃ gopemahaṃ tadā.",
"thi-ap19:60.1": "Disvā paccekasambuddhaṃ,",
"thi-ap19:60.2": "pañcalājāsatānihaṃ;",
"thi-ap19:60.3": "Datvā padumacchannāni,",
"thi-ap19:60.4": "pañca puttasatānihaṃ.",
"thi-ap19:61.1": "Patthayiṃ tepi patthesuṃ,",
"thi-ap19:61.2": "madhuṃ datvā sayambhuno;",
"thi-ap19:61.3": "Tato cutā araññehaṃ,",
"thi-ap19:61.4": "ajāyiṃ padumodare.",
"thi-ap19:62.1": "Kāsirañño mahesīhaṃ,",
"thi-ap19:62.2": "hutvā sakkatapūjitā;",
"thi-ap19:62.3": "Ajaniṃ rājaputtānaṃ,",
"thi-ap19:62.4": "anūnaṃ satapañcakaṃ.",
"thi-ap19:63.1": "Yadā te yobbanappattā,",
"thi-ap19:63.2": "kīḷantā jalakīḷitaṃ;",
"thi-ap19:63.3": "Disvā opattapadumaṃ,",
"thi-ap19:63.4": "āsuṃ paccekanāyakā.",
"thi-ap19:64.1": "Sāhaṃ tehi vinābhūtā,",
"thi-ap19:64.2": "sutavīrehi sokinī;",
"thi-ap19:64.3": "Cutā isigilipasse,",
"thi-ap19:64.4": "gāmakamhi ajāyihaṃ.",
"thi-ap19:65.1": "Yadā buddhā sutamatī,",
"thi-ap19:65.2": "sutānaṃ bhattunopi ca;",
"thi-ap19:65.3": "Yāguṃ ādāya gacchantī,",
"thi-ap19:65.4": "aṭṭha paccekanāyake.",
"thi-ap19:66.1": "Bhikkhāya gāmaṃ gacchante,",
"thi-ap19:66.2": "disvā putte anussariṃ;",
"thi-ap19:66.3": "Khīradhārā viniggacchi,",
"thi-ap19:66.4": "tadā me puttapemasā.",
"thi-ap19:67.1": "Tato tesaṃ adaṃ yāguṃ,",
"thi-ap19:67.2": "pasannā sehi pāṇibhi;",
"thi-ap19:67.3": "Tato cutāhaṃ tidasaṃ,",
"thi-ap19:67.4": "nandanaṃ upapajjahaṃ.",
"thi-ap19:68.1": "Anubhotvā sukhaṃ dukkhaṃ,",
"thi-ap19:68.2": "saṃsaritvā bhavābhave;",
"thi-ap19:68.3": "Tavatthāya mahāvīra,",
"thi-ap19:68.4": "pariccattañca jīvitaṃ.",
"thi-ap19:69.1": "Evaṃ bahuvidhaṃ dukkhaṃ,",
"thi-ap19:69.2": "sampattī ca bahubbidhā;",
"thi-ap19:69.3": "Pacchime bhave sampatte,",
"thi-ap19:69.4": "jātā sāvatthiyaṃ pure.",
"thi-ap19:70.1": "Mahādhanaseṭṭhikule,",
"thi-ap19:70.2": "sukhite sajjite tathā;",
"thi-ap19:70.3": "Nānāratanapajjote,",
"thi-ap19:70.4": "sabbakāmasamiddhine.",
"thi-ap19:71.1": "Sakkatā pūjitā ceva,",
"thi-ap19:71.2": "mānitāpacitā tathā;",
"thi-ap19:71.3": "Rūpasobhaggasampannā,",
"thi-ap19:71.4": "kulesu atisakkatā.",
"thi-ap19:72.1": "Atīva patthitā cāsiṃ,",
"thi-ap19:72.2": "rūpabhogasirīhi ca;",
"thi-ap19:72.3": "Patthitā seṭṭhiputtehi,",
"thi-ap19:72.4": "anekehi satehipi.",
"thi-ap19:73.1": "Agāraṃ pajahitvāna,",
"thi-ap19:73.2": "pabbajiṃ anagāriyaṃ;",
"thi-ap19:73.3": "Aḍḍhamāse asampatte,",
"thi-ap19:73.4": "catusaccamapāpuṇiṃ.",
"thi-ap19:74.1": "Iddhiyā abhinimmitvā,",
"thi-ap19:74.2": "caturassaṃ rathaṃ ahaṃ;",
"thi-ap19:74.3": "Buddhassa pāde vandissaṃ,",
"thi-ap19:74.4": "lokanāthassa tādino.",
"thi-ap19:75.1": "‘Supupphitaggaṃ upagamma pādapaṃ,",
"thi-ap19:75.2": "Ekā tuvaṃ tiṭṭhasi sālamūle;",
"thi-ap19:75.3": "Na cāpi te dutiyo atthi koci,",
"thi-ap19:75.4": "Bāle na tvaṃ bhāyasi dhuttakānaṃ’.",
"thi-ap19:76.1": "‘Sataṃ sahassānipi dhuttakānaṃ,",
"thi-ap19:76.2": "Samāgatā edisakā bhaveyyuṃ;",
"thi-ap19:76.3": "Lomaṃ na iñje na sampavedhe,",
"thi-ap19:76.4": "Kiṃ me tuvaṃ māra karissaseko.",
"thi-ap19:77.1": "Esā antaradhāyāmi,",
"thi-ap19:77.2": "kucchiṃ vā pavisāmi te;",
"thi-ap19:77.3": "Bhamukantarikāyampi,",
"thi-ap19:77.4": "tiṭṭhantiṃ maṃ na dakkhasi.",
"thi-ap19:78.1": "Cittasmiṃ vasībhūtāmhi,",
"thi-ap19:78.2": "iddhipādā subhāvitā;",
"thi-ap19:78.3": "Sabbabandhanamuttāmhi,",
"thi-ap19:78.4": "na taṃ bhāyāmi āvuso.",
"thi-ap19:79.1": "Sattisūlūpamā kāmā,",
"thi-ap19:79.2": "khandhāsaṃ adhikuṭṭanā;",
"thi-ap19:79.3": "Yaṃ tvaṃ kāmaratiṃ brūsi,",
"thi-ap19:79.4": "aratī dāni sā mama.",
"thi-ap19:80.1": "Sabbattha vihatā nandī,",
"thi-ap19:80.2": "tamokhandho padālito;",
"thi-ap19:80.3": "Evaṃ jānāhi pāpima,",
"thi-ap19:80.4": "nihato tvamasi antaka’.",
"thi-ap19:81.1": "Jino tamhi guṇe tuṭṭho,",
"thi-ap19:81.2": "etadagge ṭhapesi maṃ;",
"thi-ap19:81.3": "Aggā iddhimatīnanti,",
"thi-ap19:81.4": "parisāsu vināyako.",
"thi-ap19:82.1": "Pariciṇṇo mayā satthā,",
"thi-ap19:82.2": "kataṃ buddhassa sāsanaṃ;",
"thi-ap19:82.3": "Ohito garuko bhāro,",
"thi-ap19:82.4": "bhavanettisamūhatā.",
"thi-ap19:83.1": "Yassatthāya pabbajitā,",
"thi-ap19:83.2": "agārasmānagāriyaṃ;",
"thi-ap19:83.3": "So me attho anuppatto,",
"thi-ap19:83.4": "sabbasaṃyojanakkhayo.",
"thi-ap19:84.1": "Cīvaraṃ piṇḍapātañca,",
"thi-ap19:84.2": "paccayaṃ sayanāsanaṃ;",
"thi-ap19:84.3": "Khaṇena upanāmenti,",
"thi-ap19:84.4": "sahassāni samantato.",
"thi-ap19:85.1": "Kilesā jhāpitā mayhaṃ, … pe …",
"thi-ap19:85.4": "viharāmi anāsavā.",
"thi-ap19:86.1": "Svāgataṃ vata me āsi, … pe …",
"thi-ap19:86.4": "kataṃ buddhassa sāsanaṃ.",
"thi-ap19:87.1": "Paṭisambhidā catasso, … pe …",
"thi-ap19:87.4": "kataṃ buddhassa sāsanaṃ”.",
"thi-ap19:88.1": "Itthaṃ sudaṃ uppalavaṇṇā bhikkhunī imā gāthāyo abhāsitthāti.",
"thi-ap19:89.1": "Uppalavaṇṇātheriyāpadānaṃ navamaṃ."
}