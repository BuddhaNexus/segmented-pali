{
"thi-ap17:1.1": "Therīapadāna",
"thi-ap17:2.1": "Ekūposathikavagga",
"thi-ap17:3.1": "7. Mahāpajāpatigotamītherīapadāna",
"thi-ap17:4.1": "Ekadā lokapajjoto,",
"thi-ap17:4.2": "vesāliyaṃ mahāvane;",
"thi-ap17:4.3": "Kūṭāgāre susālāyaṃ,",
"thi-ap17:4.4": "vasate narasārathi.",
"thi-ap17:5.1": "Tadā jinassa mātucchā,",
"thi-ap17:5.2": "mahāgotami bhikkhunī;",
"thi-ap17:5.3": "Tahiṃ kate pure ramme,",
"thi-ap17:5.4": "vasī bhikkhunupassaye.",
"thi-ap17:6.1": "Bhikkhunīhi vimuttāhi,",
"thi-ap17:6.2": "satehi saha pañcahi;",
"thi-ap17:6.3": "Rahogatāya tassevaṃ,",
"thi-ap17:6.4": "citassāsi vitakkitaṃ.",
"thi-ap17:7.1": "“Buddhassa parinibbānaṃ,",
"thi-ap17:7.2": "sāvakaggayugassa vā;",
"thi-ap17:7.3": "Rāhulānandanandānaṃ,",
"thi-ap17:7.4": "nāhaṃ lacchāmi passituṃ.",
"thi-ap17:8.1": "Buddhassa parinibbānā,",
"thi-ap17:8.2": "sāvakaggayugassa vā;",
"thi-ap17:8.3": "Mahākassapanandānaṃ,",
"thi-ap17:8.4": "ānandarāhulāna ca.",
"thi-ap17:9.1": "Paṭikaccāyusaṅkhāraṃ,",
"thi-ap17:9.2": "osajjitvāna nibbutiṃ;",
"thi-ap17:9.3": "Gaccheyyaṃ lokanāthena,",
"thi-ap17:9.4": "anuññātā mahesinā”.",
"thi-ap17:10.1": "Tathā pañcasatānampi,",
"thi-ap17:10.2": "bhikkhunīnaṃ vitakkitaṃ;",
"thi-ap17:10.3": "Āsi khemādikānampi,",
"thi-ap17:10.4": "etadeva vitakkitaṃ.",
"thi-ap17:11.1": "Bhūmicālo tadā āsi,",
"thi-ap17:11.2": "nāditā devadundubhī;",
"thi-ap17:11.3": "Upassayādhivatthāyo,",
"thi-ap17:11.4": "devatā sokapīḷitā.",
"thi-ap17:12.1": "Vilapantā sukaruṇaṃ,",
"thi-ap17:12.2": "tatthassūni pavattayuṃ;",
"thi-ap17:12.3": "Mittā bhikkhuniyo tāhi,",
"thi-ap17:12.4": "upagantvāna gotamiṃ.",
"thi-ap17:13.1": "Nipacca sirasā pāde,",
"thi-ap17:13.2": "idaṃ vacanamabravuṃ;",
"thi-ap17:13.3": "Tattha toyalavāsittā,",
"thi-ap17:13.4": "mayamayye rahogatā.",
"thi-ap17:14.1": "Sā calā calitā bhūmi,",
"thi-ap17:14.2": "nāditā devadundubhī;",
"thi-ap17:14.3": "Paridevā ca suyyante,",
"thi-ap17:14.4": "kimatthaṃ nūna gotamī.",
"thi-ap17:15.1": "Tadā avoca sā sabbaṃ,",
"thi-ap17:15.2": "yathāparivitakkitaṃ;",
"thi-ap17:15.3": "Tāyopi sabbā āhaṃsu,",
"thi-ap17:15.4": "yathāparivitakkitaṃ.",
"thi-ap17:16.1": "“Yadi te rucitaṃ ayye,",
"thi-ap17:16.2": "nibbānaṃ paramaṃ sivaṃ;",
"thi-ap17:16.3": "Nibbāyissāma sabbāpi,",
"thi-ap17:16.4": "buddhānuññāya subbate.",
"thi-ap17:17.1": "Mayaṃ sahāva nikkhantā,",
"thi-ap17:17.2": "gharāpi ca bhavāpi ca;",
"thi-ap17:17.3": "Sahāyeva gamissāma,",
"thi-ap17:17.4": "nibbānaṃ padamuttamaṃ”.",
"thi-ap17:18.1": "“Nibbānāya vajantīnaṃ,",
"thi-ap17:18.2": "kiṃ vakkhāmī”ti sā vadaṃ;",
"thi-ap17:18.3": "Saha sabbāhi niggañchi,",
"thi-ap17:18.4": "bhikkhunīnilayā tadā.",
"thi-ap17:19.1": "“Upassaye yādhivatthā,",
"thi-ap17:19.2": "devatā tā khamantu me;",
"thi-ap17:19.3": "Bhikkhunīnilayassedaṃ,",
"thi-ap17:19.4": "pacchimaṃ dassanaṃ mama.",
"thi-ap17:20.1": "Na jarā maccu vā yattha,",
"thi-ap17:20.2": "appiyehi samāgamo;",
"thi-ap17:20.3": "Piyehi na viyogotthi,",
"thi-ap17:20.4": "taṃ vajissaṃ asaṅkhataṃ”.",
"thi-ap17:21.1": "Avītarāgā taṃ sutvā,",
"thi-ap17:21.2": "vacanaṃ sugatorasā;",
"thi-ap17:21.3": "Sokaṭṭā parideviṃsu,",
"thi-ap17:21.4": "“aho no appapuññatā.",
"thi-ap17:22.1": "Bhikkhunīnilayo suñño,",
"thi-ap17:22.2": "bhūto tāhi vinā ayaṃ;",
"thi-ap17:22.3": "Pabhāte viya tārāyo,",
"thi-ap17:22.4": "na dissanti jinorasā.",
"thi-ap17:23.1": "Nibbānaṃ gotamī yāti,",
"thi-ap17:23.2": "satehi saha pañcahi;",
"thi-ap17:23.3": "Nadīsatehiva saha,",
"thi-ap17:23.4": "gaṅgā pañcahi sāgaraṃ”.",
"thi-ap17:24.1": "Rathiyāya vajantiyo,",
"thi-ap17:24.2": "disvā saddhā upāsikā;",
"thi-ap17:24.3": "Gharā nikkhamma pādesu,",
"thi-ap17:24.4": "nipacca idamabravuṃ.",
"thi-ap17:25.1": "“Pasīdassu mahābhoge,",
"thi-ap17:25.2": "anāthāyo vihāya no;",
"thi-ap17:25.3": "Tayā na yuttā nibbātuṃ,",
"thi-ap17:25.4": "icchaṭṭā vilapiṃsu tā”.",
"thi-ap17:26.1": "Tāsaṃ sokapahānatthaṃ,",
"thi-ap17:26.2": "avoca madhuraṃ giraṃ;",
"thi-ap17:26.3": "“Ruditena alaṃ puttā,",
"thi-ap17:26.4": "hāsakāloyamajja vo.",
"thi-ap17:27.1": "Pariññātaṃ mayā dukkhaṃ,",
"thi-ap17:27.2": "dukkhahetu vivajjito;",
"thi-ap17:27.3": "Nirodho me sacchikato,",
"thi-ap17:27.4": "maggo cāpi subhāvito.",
"thi-ap17:28.1": "Paṭhamaṃ bhāṇavāraṃ.",
"thi-ap17:29.1": "Pariciṇṇo mayā satthā,",
"thi-ap17:29.2": "kataṃ buddhassa sāsanaṃ;",
"thi-ap17:29.3": "Ohito garuko bhāro,",
"thi-ap17:29.4": "bhavanetti samūhatā.",
"thi-ap17:30.1": "Yassatthāya pabbajitā,",
"thi-ap17:30.2": "agārasmānagāriyaṃ;",
"thi-ap17:30.3": "So me attho anuppatto,",
"thi-ap17:30.4": "sabbasaṃyojanakkhayo.",
"thi-ap17:31.1": "Buddho tassa ca saddhammo,",
"thi-ap17:31.2": "anūno yāva tiṭṭhati;",
"thi-ap17:31.3": "Nibbātuṃ tāva kālo me,",
"thi-ap17:31.4": "mā maṃ socatha puttikā.",
"thi-ap17:32.1": "Koṇḍaññānandanandādī,",
"thi-ap17:32.2": "tiṭṭhanti rāhulo jino;",
"thi-ap17:32.3": "Sukhito sahito saṃgho,",
"thi-ap17:32.4": "hatadabbā ca titthiyā.",
"thi-ap17:33.1": "Okkākavaṃsassa yaso,",
"thi-ap17:33.2": "ussito māramaddano;",
"thi-ap17:33.3": "Nanu sampati kālo me,",
"thi-ap17:33.4": "nibbānatthāya puttikā.",
"thi-ap17:34.1": "Cirappabhuti yaṃ mayhaṃ,",
"thi-ap17:34.2": "patthitaṃ ajja sijjhate;",
"thi-ap17:34.3": "Ānandabherikāloyaṃ,",
"thi-ap17:34.4": "kiṃ vo assūhi puttikā.",
"thi-ap17:35.1": "Sace mayi dayā atthi,",
"thi-ap17:35.2": "yadi catthi kataññutā;",
"thi-ap17:35.3": "Saddhammaṭṭhitiyā sabbā,",
"thi-ap17:35.4": "karotha vīriyaṃ daḷhaṃ.",
"thi-ap17:36.1": "Thīnaṃ adāsi pabbajjaṃ,",
"thi-ap17:36.2": "sambuddho yācito mayā;",
"thi-ap17:36.3": "Tasmā yathāhaṃ nandissaṃ,",
"thi-ap17:36.4": "tathā tamanutiṭṭhatha”.",
"thi-ap17:37.1": "Tā evamanusāsitvā,",
"thi-ap17:37.2": "bhikkhunīhi purakkhatā;",
"thi-ap17:37.3": "Upecca buddhaṃ vanditvā,",
"thi-ap17:37.4": "idaṃ vacanamabravi.",
"thi-ap17:38.1": "“Ahaṃ sugata te mātā,",
"thi-ap17:38.2": "tvañca vīra pitā mama;",
"thi-ap17:38.3": "Saddhammasukhada nātha,",
"thi-ap17:38.4": "tayā jātāmhi gotama.",
"thi-ap17:39.1": "Saṃvaddhitoyaṃ sugata,",
"thi-ap17:39.2": "rūpakāyo mayā tava;",
"thi-ap17:39.3": "Anindito dhammakāyo,",
"thi-ap17:39.4": "mama saṃvaddhito tayā.",
"thi-ap17:40.1": "Muhuttaṃ taṇhāsamaṇaṃ,",
"thi-ap17:40.2": "khīraṃ tvaṃ pāyito mayā;",
"thi-ap17:40.3": "Tayāhaṃ santamaccantaṃ,",
"thi-ap17:40.4": "dhammakhīrañhi pāyitā.",
"thi-ap17:41.1": "Bandhanārakkhane mayhaṃ,",
"thi-ap17:41.2": "anaṇo tvaṃ mahāmune;",
"thi-ap17:41.3": "Puttakāmā thiyo yācaṃ,",
"thi-ap17:41.4": "labhanti tādisaṃ sutaṃ.",
"thi-ap17:42.1": "Mandhātādinarindānaṃ,",
"thi-ap17:42.2": "yā mātā sā bhavaṇṇave;",
"thi-ap17:42.3": "Nimuggāhaṃ tayā putta,",
"thi-ap17:42.4": "tāritā bhavasāgarā.",
"thi-ap17:43.1": "Rañño mātā mahesīti,",
"thi-ap17:43.2": "sulabhaṃ nāmamitthinaṃ;",
"thi-ap17:43.3": "Buddhamātāti yaṃ nāmaṃ,",
"thi-ap17:43.4": "etaṃ paramadullabhaṃ.",
"thi-ap17:44.1": "Tañca laddhaṃ mahāvīra,",
"thi-ap17:44.2": "paṇidhānaṃ mamaṃ tayā;",
"thi-ap17:44.3": "Aṇukaṃ vā mahantaṃ vā,",
"thi-ap17:44.4": "taṃ sabbaṃ pūritaṃ mayā.",
"thi-ap17:45.1": "Parinibbātumicchāmi,",
"thi-ap17:45.2": "vihāyemaṃ kaḷevaraṃ;",
"thi-ap17:45.3": "Anujānāhi me vīra,",
"thi-ap17:45.4": "dukkhantakara nāyaka.",
"thi-ap17:46.1": "Cakkaṅkusadhajākiṇṇe,",
"thi-ap17:46.2": "pāde kamalakomale;",
"thi-ap17:46.3": "Pasārehi paṇāmaṃ te,",
"thi-ap17:46.4": "karissaṃ puttauttame.",
"thi-ap17:47.1": "Suvaṇṇarāsisaṅkāsaṃ,",
"thi-ap17:47.2": "sarīraṃ kuru pākaṭaṃ;",
"thi-ap17:47.3": "Katvā dehaṃ sudiṭṭhaṃ te,",
"thi-ap17:47.4": "santiṃ gacchāmi nāyaka”.",
"thi-ap17:48.1": "Dvattiṃsalakkhaṇūpetaṃ,",
"thi-ap17:48.2": "suppabhālaṅkataṃ tanuṃ;",
"thi-ap17:48.3": "Sañjhāghanāva bālakkaṃ,",
"thi-ap17:48.4": "mātucchaṃ dassayī jino.",
"thi-ap17:49.1": "Phullāravindasaṅkāse,",
"thi-ap17:49.2": "taruṇādiccasappabhe;",
"thi-ap17:49.3": "Cakkaṅkite pādatale,",
"thi-ap17:49.4": "tato sā sirasā pati.",
"thi-ap17:50.1": "“Paṇamāmi narādicca,",
"thi-ap17:50.2": "ādiccakulaketukaṃ;",
"thi-ap17:50.3": "Pacchime maraṇe mayhaṃ,",
"thi-ap17:50.4": "na taṃ ikkhāmahaṃ puno.",
"thi-ap17:51.1": "Itthiyo nāma lokagga,",
"thi-ap17:51.2": "sabbadosākarā matā;",
"thi-ap17:51.3": "Yadi ko catthi doso me,",
"thi-ap17:51.4": "khamassu karuṇākara.",
"thi-ap17:52.1": "Itthikānañca pabbajjaṃ,",
"thi-ap17:52.2": "haṃ taṃ yāciṃ punappunaṃ;",
"thi-ap17:52.3": "Tattha ce atthi doso me,",
"thi-ap17:52.4": "taṃ khamassu narāsabha.",
"thi-ap17:53.1": "Mayā bhikkhuniyo vīra,",
"thi-ap17:53.2": "tavānuññāya sāsitā;",
"thi-ap17:53.3": "Tatra ce atthi dunnītaṃ,",
"thi-ap17:53.4": "taṃ khamassu khamādhipa”.",
"thi-ap17:54.1": "“Akkhante nāma khantabbaṃ,",
"thi-ap17:54.2": "kiṃ bhave guṇabhūsane;",
"thi-ap17:54.3": "Kimuttaraṃ te vakkhāmi,",
"thi-ap17:54.4": "nibbānāya vajantiyā.",
"thi-ap17:55.1": "Suddhe anūne mama bhikkhusaṃghe,",
"thi-ap17:55.2": "Lokā ito nissarituṃ khamante;",
"thi-ap17:55.3": "Pabhātakāle byasanaṅgatānaṃ,",
"thi-ap17:55.4": "Disvāna niyyātiva candalekhā”.",
"thi-ap17:56.1": "Tadetarā bhikkhuniyo jinaggaṃ,",
"thi-ap17:56.2": "Tārāva candānugatā sumeruṃ;",
"thi-ap17:56.3": "Padakkhiṇaṃ kacca nipacca pāde,",
"thi-ap17:56.4": "Ṭhitā mukhantaṃ samudikkhamānā.",
"thi-ap17:57.1": "“Na tittipubbaṃ tava dassanena,",
"thi-ap17:57.2": "Cakkhuṃ na sotaṃ tava bhāsitena;",
"thi-ap17:57.3": "Cittaṃ mamaṃ kevalamekameva,",
"thi-ap17:57.4": "Pappuyya taṃ dhammarasena titti.",
"thi-ap17:58.1": "Nadato parisāyaṃ te,",
"thi-ap17:58.2": "vāditabbapahārino;",
"thi-ap17:58.3": "Ye te dakkhanti vadanaṃ,",
"thi-ap17:58.4": "dhaññā te narapuṅgava.",
"thi-ap17:59.1": "Dīghaṅgulī tambanakhe,",
"thi-ap17:59.2": "subhe āyatapaṇhike;",
"thi-ap17:59.3": "Ye pāde paṇamissanti,",
"thi-ap17:59.4": "tepi dhaññā guṇandhara.",
"thi-ap17:60.1": "Madhurāni pahaṭṭhāni,",
"thi-ap17:60.2": "dosagghāni hitāni ca;",
"thi-ap17:60.3": "Ye te vākyāni suyyanti,",
"thi-ap17:60.4": "tepi dhaññā naruttama.",
"thi-ap17:61.1": "Dhaññāhaṃ te mahāvīra,",
"thi-ap17:61.2": "pādapūjanatapparā;",
"thi-ap17:61.3": "Tiṇṇasaṃsārakantārā,",
"thi-ap17:61.4": "suvākyena sirīmato”.",
"thi-ap17:62.1": "Tato sā anusāvetvā,",
"thi-ap17:62.2": "bhikkhusaṃghampi subbatā;",
"thi-ap17:62.3": "Rāhulānandanande ca,",
"thi-ap17:62.4": "vanditvā idamabravi.",
"thi-ap17:63.1": "“Āsīvisālayasame,",
"thi-ap17:63.2": "rogāvāse kaḷevare;",
"thi-ap17:63.3": "Nibbindā dukkhasaṅghāṭe,",
"thi-ap17:63.4": "jarāmaraṇagocare.",
"thi-ap17:64.1": "Nānākalimalākiṇṇe,",
"thi-ap17:64.2": "parāyatte nirīhake;",
"thi-ap17:64.3": "Tena nibbātumicchāmi,",
"thi-ap17:64.4": "anumaññatha puttakā”.",
"thi-ap17:65.1": "Nando rāhulabhaddo ca,",
"thi-ap17:65.2": "vītasokā nirāsavā;",
"thi-ap17:65.3": "Ṭhitācalaṭṭhiti thirā,",
"thi-ap17:65.4": "dhammatamanucintayuṃ.",
"thi-ap17:66.1": "“Dhiratthu saṅkhataṃ lolaṃ,",
"thi-ap17:66.2": "asāraṃ kadalūpamaṃ;",
"thi-ap17:66.3": "Māyāmarīcisadisaṃ,",
"thi-ap17:66.4": "ittaraṃ anavaṭṭhitaṃ.",
"thi-ap17:67.1": "Yattha nāma jinassāyaṃ,",
"thi-ap17:67.2": "mātucchā buddhaposikā;",
"thi-ap17:67.3": "Gotamī nidhanaṃ yāti,",
"thi-ap17:67.4": "aniccaṃ sabbasaṅkhataṃ”.",
"thi-ap17:68.1": "Ānando ca tadā sekho,",
"thi-ap17:68.2": "sokaṭṭo jinavacchalo;",
"thi-ap17:68.3": "Tatthassūni karonto so,",
"thi-ap17:68.4": "karuṇaṃ paridevati.",
"thi-ap17:69.1": "“Hā santiṃ gotamī yāti,",
"thi-ap17:69.2": "nūna buddhopi nibbutiṃ;",
"thi-ap17:69.3": "Gacchati na cireneva,",
"thi-ap17:69.4": "aggiriva nirindhano”.",
"thi-ap17:70.1": "Evaṃ vilāpamānaṃ taṃ,",
"thi-ap17:70.2": "ānandaṃ āha gotamī;",
"thi-ap17:70.3": "Sutasāgaragambhīra-",
"thi-ap17:70.4": "buddhopaṭṭhānatappara.",
"thi-ap17:71.1": "“Na yuttaṃ socituṃ putta,",
"thi-ap17:71.2": "hāsakāle upaṭṭhite;",
"thi-ap17:71.3": "Tayā me saraṇaṃ putta,",
"thi-ap17:71.4": "nibbānaṃ tamupāgataṃ.",
"thi-ap17:72.1": "Tayā tāta samajjhiṭṭho,",
"thi-ap17:72.2": "pabbajjaṃ anujāni no;",
"thi-ap17:72.3": "Mā putta vimano hohi,",
"thi-ap17:72.4": "saphalo te parissamo.",
"thi-ap17:73.1": "Yaṃ na diṭṭhaṃ purāṇehi,",
"thi-ap17:73.2": "titthikācariyehipi;",
"thi-ap17:73.3": "Taṃ padaṃ sukumārīhi,",
"thi-ap17:73.4": "sattavassāhi veditaṃ.",
"thi-ap17:74.1": "Buddhasāsanapāleta,",
"thi-ap17:74.2": "pacchimaṃ dassanaṃ tava;",
"thi-ap17:74.3": "Tattha gacchāmahaṃ putta,",
"thi-ap17:74.4": "gato yattha na dissate.",
"thi-ap17:75.1": "Kadāci dhammaṃ desento,",
"thi-ap17:75.2": "khipī lokagganāyako;",
"thi-ap17:75.3": "Tadāhaṃ āsīsavācaṃ,",
"thi-ap17:75.4": "avocaṃ anukampikā.",
"thi-ap17:76.1": "‘Ciraṃ jīva mahāvīra,",
"thi-ap17:76.2": "kappaṃ tiṭṭha mahāmune;",
"thi-ap17:76.3": "Sabbalokassa atthāya,",
"thi-ap17:76.4": "bhavassu ajarāmaro’.",
"thi-ap17:77.1": "Taṃ tathāvādiniṃ buddho,",
"thi-ap17:77.2": "mamaṃ so etadabravi;",
"thi-ap17:77.3": "‘Na hevaṃ vandiyā buddhā,",
"thi-ap17:77.4": "yathā vandasi gotamī’.",
"thi-ap17:78.1": "‘Kathaṃ carahi sabbaññū,",
"thi-ap17:78.2": "vanditabbā tathāgatā;",
"thi-ap17:78.3": "Kathaṃ avandiyā buddhā,",
"thi-ap17:78.4": "taṃ me akkhāhi pucchito’.",
"thi-ap17:79.1": "‘Āraddhavīriye pahitatte,",
"thi-ap17:79.2": "Niccaṃ daḷhaparakkame;",
"thi-ap17:79.3": "Samagge sāvake passa,",
"thi-ap17:79.4": "Etaṃ buddhānavandanaṃ’.",
"thi-ap17:80.1": "Tato upassayaṃ gantvā,",
"thi-ap17:80.2": "ekikāhaṃ vicintayiṃ;",
"thi-ap17:80.3": "‘Samaggaparisaṃ nātho,",
"thi-ap17:80.4": "rodhesi tibhavantago.",
"thi-ap17:81.1": "Handāhaṃ parinibbissaṃ,",
"thi-ap17:81.2": "mā vipattitamaddasaṃ’;",
"thi-ap17:81.3": "Evāhaṃ cintayitvāna,",
"thi-ap17:81.4": "disvāna isisattamaṃ.",
"thi-ap17:82.1": "Parinibbānakālaṃ me,",
"thi-ap17:82.2": "ārocesiṃ vināyakaṃ;",
"thi-ap17:82.3": "Tato so samanuññāsi,",
"thi-ap17:82.4": "‘kālaṃ jānāhi gotamī’.",
"thi-ap17:83.1": "Kilesā jhāpitā mayhaṃ, … pe …",
"thi-ap17:83.4": "viharāmi anāsavā.",
"thi-ap17:84.1": "Svāgataṃ vata me āsi, … pe …",
"thi-ap17:84.4": "kataṃ buddhassa sāsanaṃ.",
"thi-ap17:85.1": "Paṭisambhidā catasso, … pe …",
"thi-ap17:85.4": "kataṃ buddhassa sāsanaṃ”.",
"thi-ap17:86.1": "“Thīnaṃ dhammābhisamaye,",
"thi-ap17:86.2": "ye bālā vimatiṃ gatā;",
"thi-ap17:86.3": "Tesaṃ diṭṭhippahānatthaṃ,",
"thi-ap17:86.4": "iddhiṃ dassehi gotamī”.",
"thi-ap17:87.1": "Tadā nipacca sambuddhaṃ,",
"thi-ap17:87.2": "uppatitvāna ambaraṃ;",
"thi-ap17:87.3": "Iddhī anekā dassesi,",
"thi-ap17:87.4": "buddhānuññāya gotamī.",
"thi-ap17:88.1": "Ekikā bahudhā āsi,",
"thi-ap17:88.2": "bahukā cekikā tathā;",
"thi-ap17:88.3": "Āvibhāvaṃ tirobhāvaṃ,",
"thi-ap17:88.4": "tirokuṭṭaṃ tironagaṃ.",
"thi-ap17:89.1": "Asajjamānā agamā,",
"thi-ap17:89.2": "bhūmiyampi nimujjatha;",
"thi-ap17:89.3": "Abhijjamāne udake,",
"thi-ap17:89.4": "agañchi mahiyā yathā.",
"thi-ap17:90.1": "Sakuṇīva tathākāse,",
"thi-ap17:90.2": "pallaṅkena kamī tadā;",
"thi-ap17:90.3": "Vasaṃ vattesi kāyena,",
"thi-ap17:90.4": "yāva brahmanivesanaṃ.",
"thi-ap17:91.1": "Sineruṃ daṇḍaṃ katvāna,",
"thi-ap17:91.2": "chattaṃ katvā mahāmahiṃ;",
"thi-ap17:91.3": "Samūlaṃ parivattetvā,",
"thi-ap17:91.4": "dhārayaṃ caṅkamī nabhe.",
"thi-ap17:92.1": "Chassūrodayakāleva,",
"thi-ap17:92.2": "lokañcākāsi dhūmikaṃ;",
"thi-ap17:92.3": "Yugante viya lokaṃ sā,",
"thi-ap17:92.4": "jālāmālākulaṃ akā.",
"thi-ap17:93.1": "Mucalindaṃ mahāselaṃ,",
"thi-ap17:93.2": "merumūlanadantare;",
"thi-ap17:93.3": "Sāsapāriva sabbāni,",
"thi-ap17:93.4": "ekenaggahi muṭṭhinā.",
"thi-ap17:94.1": "Aṅgulaggena chādesi,",
"thi-ap17:94.2": "bhākaraṃ sanisākaraṃ;",
"thi-ap17:94.3": "Candasūrasahassāni,",
"thi-ap17:94.4": "āveḷamiva dhārayi.",
"thi-ap17:95.1": "Catusāgaratoyāni,",
"thi-ap17:95.2": "dhārayī ekapāṇinā;",
"thi-ap17:95.3": "Yugantajaladākāraṃ,",
"thi-ap17:95.4": "mahāvassaṃ pavassatha.",
"thi-ap17:96.1": "Cakkavattiṃ saparisaṃ,",
"thi-ap17:96.2": "māpayī sā nabhattale;",
"thi-ap17:96.3": "Garuḷaṃ dviradaṃ sīhaṃ,",
"thi-ap17:96.4": "vinadantaṃ padassayi.",
"thi-ap17:97.1": "Ekikā abhinimmitvā,",
"thi-ap17:97.2": "Appameyyaṃ bhikkhunīgaṇaṃ;",
"thi-ap17:97.3": "Puna antaradhāpetvā,",
"thi-ap17:97.4": "Ekikā munimabravi.",
"thi-ap17:98.1": "“Mātucchā te mahāvīra,",
"thi-ap17:98.2": "tava sāsanakārikā;",
"thi-ap17:98.3": "Anuppattā sakaṃ atthaṃ,",
"thi-ap17:98.4": "pāde vandāmi cakkhuma”.",
"thi-ap17:99.1": "Dassetvā vividhā iddhī,",
"thi-ap17:99.2": "orohitvā nabhattalā;",
"thi-ap17:99.3": "Vanditvā lokapajjotaṃ,",
"thi-ap17:99.4": "ekamantaṃ nisīdi sā.",
"thi-ap17:100.1": "“Sā vīsavassasatikā,",
"thi-ap17:100.2": "jātiyāhaṃ mahāmune;",
"thi-ap17:100.3": "Alamettāvatā vīra,",
"thi-ap17:100.4": "nibbāyissāmi nāyaka”.",
"thi-ap17:101.1": "Tadātivimhitā sabbā,",
"thi-ap17:101.2": "parisā sā katañjalī;",
"thi-ap17:101.3": "“Avocayye kathaṃ āsi,",
"thi-ap17:101.4": "atuliddhiparakkamā”.",
"thi-ap17:102.1": "“Padumuttaro nāma jino,",
"thi-ap17:102.2": "sabbadhammesu cakkhumā;",
"thi-ap17:102.3": "Ito satasahassamhi,",
"thi-ap17:102.4": "kappe uppajji nāyako.",
"thi-ap17:103.1": "Tadāhaṃ haṃsavatiyaṃ,",
"thi-ap17:103.2": "jātāmaccakule ahuṃ;",
"thi-ap17:103.3": "Sabbopakārasampanne,",
"thi-ap17:103.4": "iddhe phīte mahaddhane.",
"thi-ap17:104.1": "Kadāci pitunā saddhiṃ,",
"thi-ap17:104.2": "dāsīgaṇapurakkhatā;",
"thi-ap17:104.3": "Mahatā parivārena,",
"thi-ap17:104.4": "taṃ upecca narāsabhaṃ.",
"thi-ap17:105.1": "Vāsavaṃ viya vassantaṃ,",
"thi-ap17:105.2": "dhammameghaṃ anāsavaṃ;",
"thi-ap17:105.3": "Saradādiccasadisaṃ,",
"thi-ap17:105.4": "raṃsijālasamujjalaṃ.",
"thi-ap17:106.1": "Disvā cittaṃ pasādetvā,",
"thi-ap17:106.2": "sutvā cassa subhāsitaṃ;",
"thi-ap17:106.3": "Mātucchaṃ bhikkhuniṃ agge,",
"thi-ap17:106.4": "ṭhapentaṃ naranāyakaṃ.",
"thi-ap17:107.1": "Sutvā datvā mahādānaṃ,",
"thi-ap17:107.2": "sattāhaṃ tassa tādino;",
"thi-ap17:107.3": "Sasaṃghassa naraggassa,",
"thi-ap17:107.4": "paccayāni bahūni ca.",
"thi-ap17:108.1": "Nipacca pādamūlamhi,",
"thi-ap17:108.2": "taṃ ṭhānamabhipatthayiṃ;",
"thi-ap17:108.3": "Tato mahāparisatiṃ,",
"thi-ap17:108.4": "avoca isisattamo.",
"thi-ap17:109.1": "‘Yā sasaṃghaṃ abhojesi,",
"thi-ap17:109.2": "sattāhaṃ lokanāyakaṃ;",
"thi-ap17:109.3": "Tamahaṃ kittayissāmi,",
"thi-ap17:109.4": "suṇātha mama bhāsato.",
"thi-ap17:110.1": "Satasahassito kappe,",
"thi-ap17:110.2": "okkākakulasambhavo;",
"thi-ap17:110.3": "Gotamo nāma gottena,",
"thi-ap17:110.4": "satthā loke bhavissati.",
"thi-ap17:111.1": "Tassa dhammesu dāyādā,",
"thi-ap17:111.2": "orasā dhammanimmitā;",
"thi-ap17:111.3": "Gotamī nāma nāmena,",
"thi-ap17:111.4": "hessati satthu sāvikā.",
"thi-ap17:112.1": "Tassa buddhassa mātucchā,",
"thi-ap17:112.2": "jīvitāpādikā ayaṃ;",
"thi-ap17:112.3": "Rattaññūnañca aggattaṃ,",
"thi-ap17:112.4": "bhikkhunīnaṃ labhissati’.",
"thi-ap17:113.1": "Taṃ sutvāna pamoditvā,",
"thi-ap17:113.2": "yāvajīvaṃ tadā jinaṃ;",
"thi-ap17:113.3": "Paccayehi upaṭṭhitvā,",
"thi-ap17:113.4": "tato kālaṅkatā ahaṃ.",
"thi-ap17:114.1": "Tāvatiṃsesu devesu,",
"thi-ap17:114.2": "sabbakāmasamiddhisu;",
"thi-ap17:114.3": "Nibbattā dasahaṅgehi,",
"thi-ap17:114.4": "aññe abhibhaviṃ ahaṃ.",
"thi-ap17:115.1": "Rūpasaddehi gandhehi,",
"thi-ap17:115.2": "rasehi phusanehi ca;",
"thi-ap17:115.3": "Āyunāpi ca vaṇṇena,",
"thi-ap17:115.4": "sukhena yasasāpi ca.",
"thi-ap17:116.1": "Tathevādhipateyyena,",
"thi-ap17:116.2": "adhigayha virocahaṃ;",
"thi-ap17:116.3": "Ahosiṃ amarindassa,",
"thi-ap17:116.4": "mahesī dayitā tahiṃ.",
"thi-ap17:117.1": "Saṃsāre saṃsarantīhaṃ,",
"thi-ap17:117.2": "kammavāyusameritā;",
"thi-ap17:117.3": "Kāsissa rañño visaye,",
"thi-ap17:117.4": "ajāyiṃ dāsagāmake.",
"thi-ap17:118.1": "Pañcadāsasatānūnā,",
"thi-ap17:118.2": "nivasanti tahiṃ tadā;",
"thi-ap17:118.3": "Sabbesaṃ tattha yo jeṭṭho,",
"thi-ap17:118.4": "tassa jāyā ahosahaṃ.",
"thi-ap17:119.1": "Sayambhuno pañcasatā,",
"thi-ap17:119.2": "gāmaṃ piṇḍāya pāvisuṃ;",
"thi-ap17:119.3": "Te disvāna ahaṃ tuṭṭhā,",
"thi-ap17:119.4": "saha sabbāhi itthibhi.",
"thi-ap17:120.1": "Pūgā hutvāva sabbāyo,",
"thi-ap17:120.2": "catumāse upaṭṭhahuṃ;",
"thi-ap17:120.3": "Ticīvarāni datvāna,",
"thi-ap17:120.4": "saṃsarimha sasāmikā.",
"thi-ap17:121.1": "Tato cutā sabbāpi tā,",
"thi-ap17:121.2": "tāvatiṃsagatā mayaṃ;",
"thi-ap17:121.3": "Pacchime ca bhave dāni,",
"thi-ap17:121.4": "jātā devadahe pure.",
"thi-ap17:122.1": "Pitā añjanasakko me,",
"thi-ap17:122.2": "mātā mama sulakkhaṇā;",
"thi-ap17:122.3": "Tato kapilavatthusmiṃ,",
"thi-ap17:122.4": "suddhodanagharaṃ gatā.",
"thi-ap17:123.1": "Sesā sakyakule jātā,",
"thi-ap17:123.2": "sakyānaṃ gharamāgamuṃ;",
"thi-ap17:123.3": "Ahaṃ visiṭṭhā sabbāsaṃ,",
"thi-ap17:123.4": "jinassāpādikā ahuṃ.",
"thi-ap17:124.1": "Mama puttobhinikkhamma,",
"thi-ap17:124.2": "buddho āsi vināyako;",
"thi-ap17:124.3": "Pacchāhaṃ pabbajitvāna,",
"thi-ap17:124.4": "satehi saha pañcahi.",
"thi-ap17:125.1": "Sākiyānīhi dhīrāhi,",
"thi-ap17:125.2": "saha santisukhaṃ phusiṃ;",
"thi-ap17:125.3": "Ye tadā pubbajātiyaṃ,",
"thi-ap17:125.4": "amhākaṃ āsu sāmino.",
"thi-ap17:126.1": "Sahapuññassa kattāro,",
"thi-ap17:126.2": "mahāsamayakārakā;",
"thi-ap17:126.3": "Phusiṃsu arahattaṃ te,",
"thi-ap17:126.4": "sugatenānukampitā”.",
"thi-ap17:127.1": "Tadetarā bhikkhuniyo,",
"thi-ap17:127.2": "āruhiṃsu nabhattalaṃ;",
"thi-ap17:127.3": "Saṅgatā viya tārāyo,",
"thi-ap17:127.4": "virociṃsu mahiddhikā.",
"thi-ap17:128.1": "Iddhī anekā dassesuṃ,",
"thi-ap17:128.2": "piḷandhavikatiṃ yathā;",
"thi-ap17:128.3": "Kammāro kanakasseva,",
"thi-ap17:128.4": "kammaññassa susikkhito.",
"thi-ap17:129.1": "Dassetvā pāṭihīrāni,",
"thi-ap17:129.2": "vicittāni bahūni ca;",
"thi-ap17:129.3": "Tosetvā vādipavaraṃ,",
"thi-ap17:129.4": "muniṃ saparisaṃ tadā.",
"thi-ap17:130.1": "Orohitvāna gaganā,",
"thi-ap17:130.2": "vanditvā isisattamaṃ;",
"thi-ap17:130.3": "Anuññātā naraggena,",
"thi-ap17:130.4": "yathāṭhāne nisīdisuṃ.",
"thi-ap17:131.1": "“Ahonukampikā amhaṃ,",
"thi-ap17:131.2": "sabbāsaṃ cira gotamī;",
"thi-ap17:131.3": "Vāsitā tava puññehi,",
"thi-ap17:131.4": "pattā no āsavakkhayaṃ.",
"thi-ap17:132.1": "Kilesā jhāpitā amhaṃ,",
"thi-ap17:132.2": "bhavā sabbe samūhatā;",
"thi-ap17:132.3": "Nāgīva bandhanaṃ chetvā,",
"thi-ap17:132.4": "viharāma anāsavā.",
"thi-ap17:133.1": "Svāgataṃ vata no āsi,",
"thi-ap17:133.2": "buddhaseṭṭhassa santike;",
"thi-ap17:133.3": "Tisso vijjā anuppattā,",
"thi-ap17:133.4": "kataṃ buddhassa sāsanaṃ.",
"thi-ap17:134.1": "Paṭisambhidā catasso,",
"thi-ap17:134.2": "vimokkhāpi ca aṭṭhime;",
"thi-ap17:134.3": "Chaḷabhiññā sacchikatā,",
"thi-ap17:134.4": "kataṃ buddhassa sāsanaṃ.",
"thi-ap17:135.1": "Iddhīsu ca vasī homa,",
"thi-ap17:135.2": "dibbāya sotadhātuyā;",
"thi-ap17:135.3": "Cetopariyañāṇassa,",
"thi-ap17:135.4": "vasī homa mahāmune.",
"thi-ap17:136.1": "Pubbenivāsaṃ jānāma,",
"thi-ap17:136.2": "dibbacakkhu visodhitaṃ;",
"thi-ap17:136.3": "Sabbāsavaparikkhīṇā,",
"thi-ap17:136.4": "natthi dāni punabbhavā.",
"thi-ap17:137.1": "Atthe dhamme ca nerutte,",
"thi-ap17:137.2": "paṭibhāne ca vijjati;",
"thi-ap17:137.3": "Ñāṇaṃ amhaṃ mahāvīra,",
"thi-ap17:137.4": "uppannaṃ tava santike.",
"thi-ap17:138.1": "Asmābhi pariciṇṇosi,",
"thi-ap17:138.2": "mettacittā hi nāyaka;",
"thi-ap17:138.3": "Anujānāhi sabbāsaṃ,",
"thi-ap17:138.4": "nibbānāya mahāmune”.",
"thi-ap17:139.1": "“Nibbāyissāma iccevaṃ,",
"thi-ap17:139.2": "kiṃ vakkhāmi vadantiyo;",
"thi-ap17:139.3": "Yassadāni ca vo kālaṃ,",
"thi-ap17:139.4": "maññathā”ti jinobravi.",
"thi-ap17:140.1": "Gotamīādikā tāyo,",
"thi-ap17:140.2": "tadā bhikkhuniyo jinaṃ;",
"thi-ap17:140.3": "Vanditvā āsanā tamhā,",
"thi-ap17:140.4": "vuṭṭhāya agamiṃsu tā.",
"thi-ap17:141.1": "Mahatā janakāyena,",
"thi-ap17:141.2": "saha lokagganāyako;",
"thi-ap17:141.3": "Anusaṃyāyī so vīro,",
"thi-ap17:141.4": "mātucchaṃ yāvakoṭṭhakaṃ.",
"thi-ap17:142.1": "Tadā nipati pādesu,",
"thi-ap17:142.2": "gotamī lokabandhuno;",
"thi-ap17:142.3": "Saheva tāhi sabbāhi,",
"thi-ap17:142.4": "“pacchimaṃ pādavandanaṃ.",
"thi-ap17:143.1": "Idaṃ pacchimakaṃ mayhaṃ,",
"thi-ap17:143.2": "lokanāthassa dassanaṃ;",
"thi-ap17:143.3": "Na puno amatākāraṃ,",
"thi-ap17:143.4": "passissāmi mukhaṃ tava.",
"thi-ap17:144.1": "Na ca me vandanaṃ vīra,",
"thi-ap17:144.2": "tava pāde sukomale;",
"thi-ap17:144.3": "Samphusissati lokagga,",
"thi-ap17:144.4": "ajja gacchāmi nibbutiṃ”.",
"thi-ap17:145.1": "“Rūpena kiṃ tavānena,",
"thi-ap17:145.2": "diṭṭhe dhamme yathātathe;",
"thi-ap17:145.3": "Sabbaṃ saṅkhatamevetaṃ,",
"thi-ap17:145.4": "anassāsikamittaraṃ”.",
"thi-ap17:146.1": "Sā saha tāhi gantvāna,",
"thi-ap17:146.2": "bhikkhunupassayaṃ sakaṃ;",
"thi-ap17:146.3": "Aḍḍhapallaṅkamābhujja,",
"thi-ap17:146.4": "nisīdi paramāsane.",
"thi-ap17:147.1": "Tadā upāsikā tattha,",
"thi-ap17:147.2": "buddhasāsanavacchalā;",
"thi-ap17:147.3": "Tassā pavattiṃ sutvāna,",
"thi-ap17:147.4": "upesuṃ pādavandikā.",
"thi-ap17:148.1": "Karehi uraṃ pahantā,",
"thi-ap17:148.2": "chinnamūlā yathā latā;",
"thi-ap17:148.3": "Rodantā karuṇaṃ ravaṃ,",
"thi-ap17:148.4": "sokaṭṭā bhūmipātitā.",
"thi-ap17:149.1": "“Mā no saraṇade nāthe,",
"thi-ap17:149.2": "vihāya gami nibbutiṃ;",
"thi-ap17:149.3": "Nipatitvāna yācāma,",
"thi-ap17:149.4": "sabbāyo sirasā mayaṃ”.",
"thi-ap17:150.1": "Yā padhānatamā tāsaṃ,",
"thi-ap17:150.2": "saddhā paññā upāsikā;",
"thi-ap17:150.3": "Tassā sīsaṃ pamajjantī,",
"thi-ap17:150.4": "idaṃ vacanamabravi.",
"thi-ap17:151.1": "“Alaṃ puttā visādena,",
"thi-ap17:151.2": "mārapāsānuvattinā;",
"thi-ap17:151.3": "Aniccaṃ saṅkhataṃ sabbaṃ,",
"thi-ap17:151.4": "viyogantaṃ calācalaṃ”.",
"thi-ap17:152.1": "Tato sā tā visajjitvā,",
"thi-ap17:152.2": "paṭhamaṃ jhānamuttamaṃ;",
"thi-ap17:152.3": "Dutiyañca tatiyañca,",
"thi-ap17:152.4": "samāpajji catutthakaṃ.",
"thi-ap17:153.1": "Ākāsāyatanañceva,",
"thi-ap17:153.2": "viññāṇāyatanaṃ tathā;",
"thi-ap17:153.3": "Ākiñcaṃ nevasaññañca,",
"thi-ap17:153.4": "samāpajji yathākkamaṃ.",
"thi-ap17:154.1": "Paṭilomena jhānāni,",
"thi-ap17:154.2": "samāpajjittha gotamī;",
"thi-ap17:154.3": "Yāvatā paṭhamaṃ jhānaṃ,",
"thi-ap17:154.4": "tato yāvacatutthakaṃ.",
"thi-ap17:155.1": "Tato vuṭṭhāya nibbāyi,",
"thi-ap17:155.2": "dīpaccīva nirāsavā;",
"thi-ap17:155.3": "Bhūmicālo mahā āsi,",
"thi-ap17:155.4": "nabhasā vijjutā pati.",
"thi-ap17:156.1": "Panāditā dundubhiyo,",
"thi-ap17:156.2": "parideviṃsu devatā;",
"thi-ap17:156.3": "Pupphavuṭṭhī ca gaganā,",
"thi-ap17:156.4": "abhivassatha medaniṃ.",
"thi-ap17:157.1": "Kampito merurājāpi,",
"thi-ap17:157.2": "raṅgamajjhe yathā naṭo;",
"thi-ap17:157.3": "Sokena cātidīnova,",
"thi-ap17:157.4": "viravo āsi sāgaro.",
"thi-ap17:158.1": "Devā nāgāsurā brahmā,",
"thi-ap17:158.2": "saṃviggāhiṃsu taṅkhaṇe;",
"thi-ap17:158.3": "“Aniccā vata saṅkhārā,",
"thi-ap17:158.4": "yathāyaṃ vilayaṃ gatā”.",
"thi-ap17:159.1": "Yā ce maṃ parivāriṃsu,",
"thi-ap17:159.2": "satthu sāsanakārikā;",
"thi-ap17:159.3": "Tāyopi anupādānā,",
"thi-ap17:159.4": "dīpacci viya nibbutā.",
"thi-ap17:160.1": "Hā yogā vippayogantā,",
"thi-ap17:160.2": "hāniccaṃ sabbasaṅkhataṃ;",
"thi-ap17:160.3": "Hā jīvitaṃ vināsantaṃ,",
"thi-ap17:160.4": "iccāsi paridevanā.",
"thi-ap17:161.1": "Tato devā ca brahmā ca,",
"thi-ap17:161.2": "lokadhammānuvattanaṃ;",
"thi-ap17:161.3": "Kālānurūpaṃ kubbanti,",
"thi-ap17:161.4": "upetvā isisattamaṃ.",
"thi-ap17:162.1": "Tadā āmantayī satthā,",
"thi-ap17:162.2": "ānandaṃ sutasāgaraṃ;",
"thi-ap17:162.3": "“Gacchānanda nivedehi,",
"thi-ap17:162.4": "bhikkhūnaṃ mātu nibbutiṃ”.",
"thi-ap17:163.1": "Tadānando nirānando,",
"thi-ap17:163.2": "assunā puṇṇalocano;",
"thi-ap17:163.3": "Gaggarena sarenāha,",
"thi-ap17:163.4": "“samāgacchantu bhikkhavo.",
"thi-ap17:164.1": "Pubbadakkhiṇapacchāsu,",
"thi-ap17:164.2": "uttarāya ca santike;",
"thi-ap17:164.3": "Suṇantu bhāsitaṃ mayhaṃ,",
"thi-ap17:164.4": "bhikkhavo sugatorasā.",
"thi-ap17:165.1": "Yā vaḍḍhayi payattena,",
"thi-ap17:165.2": "sarīraṃ pacchimaṃ mune;",
"thi-ap17:165.3": "Sā gotamī gatā santiṃ,",
"thi-ap17:165.4": "tārāva sūriyodaye.",
"thi-ap17:166.1": "Buddhamātāti paññattiṃ,",
"thi-ap17:166.2": "ṭhapayitvā gatāsamaṃ;",
"thi-ap17:166.3": "Na yattha pañcanettopi,",
"thi-ap17:166.4": "gatiṃ dakkhati nāyako.",
"thi-ap17:167.1": "Yassatthi sugate saddhā,",
"thi-ap17:167.2": "yo ca piyo mahāmune;",
"thi-ap17:167.3": "Buddhamātussa sakkāraṃ,",
"thi-ap17:167.4": "karotu sugatoraso”.",
"thi-ap17:168.1": "Sudūraṭṭhāpi taṃ sutvā,",
"thi-ap17:168.2": "sīghamāgacchu bhikkhavo;",
"thi-ap17:168.3": "Keci buddhānubhāvena,",
"thi-ap17:168.4": "keci iddhīsu kovidā.",
"thi-ap17:169.1": "Kūṭāgāravare ramme,",
"thi-ap17:169.2": "sabbasoṇṇamaye subhe;",
"thi-ap17:169.3": "Mañcakaṃ samāropesuṃ,",
"thi-ap17:169.4": "yattha suttāsi gotamī.",
"thi-ap17:170.1": "Cattāro lokapālā te,",
"thi-ap17:170.2": "aṃsehi samadhārayuṃ;",
"thi-ap17:170.3": "Sesā sakkādikā devā,",
"thi-ap17:170.4": "kūṭāgāre samaggahuṃ.",
"thi-ap17:171.1": "Kūṭāgārāni sabbāni,",
"thi-ap17:171.2": "āsuṃ pañcasatānipi;",
"thi-ap17:171.3": "Saradādiccavaṇṇāni,",
"thi-ap17:171.4": "vissakammakatāni hi.",
"thi-ap17:172.1": "Sabbā tāpi bhikkhuniyo,",
"thi-ap17:172.2": "āsuṃ mañcesu sāyitā;",
"thi-ap17:172.3": "Devānaṃ khandhamāruḷhā,",
"thi-ap17:172.4": "niyyanti anupubbaso.",
"thi-ap17:173.1": "Sabbaso chāditaṃ āsi,",
"thi-ap17:173.2": "vitānena nabhattalaṃ;",
"thi-ap17:173.3": "Satārā candasūrā ca,",
"thi-ap17:173.4": "lañchitā kanakāmayā.",
"thi-ap17:174.1": "Paṭākā ussitānekā,",
"thi-ap17:174.2": "vitatā pupphakañcukā;",
"thi-ap17:174.3": "Ogatākāsapadumā,",
"thi-ap17:174.4": "mahiyā pupphamuggataṃ.",
"thi-ap17:175.1": "Dissanti candasūriyā,",
"thi-ap17:175.2": "pajjalanti ca tārakā;",
"thi-ap17:175.3": "Majjhaṃ gatopi cādicco,",
"thi-ap17:175.4": "na tāpesi sasī yathā.",
"thi-ap17:176.1": "Devā dibbehi gandhehi,",
"thi-ap17:176.2": "mālehi surabhīhi ca;",
"thi-ap17:176.3": "Vāditehi ca naccehi,",
"thi-ap17:176.4": "saṅgītīhi ca pūjayuṃ.",
"thi-ap17:177.1": "Nāgāsurā ca brahmāno,",
"thi-ap17:177.2": "yathāsatti yathābalaṃ;",
"thi-ap17:177.3": "Pūjayiṃsu ca niyyantiṃ,",
"thi-ap17:177.4": "nibbutaṃ buddhamātaraṃ.",
"thi-ap17:178.1": "Sabbāyo purato nītā,",
"thi-ap17:178.2": "nibbutā sugatorasā;",
"thi-ap17:178.3": "Gotamī niyyate pacchā,",
"thi-ap17:178.4": "sakkatā buddhaposikā.",
"thi-ap17:179.1": "Purato devamanujā,",
"thi-ap17:179.2": "sanāgāsurabrahmakā;",
"thi-ap17:179.3": "Pacchā sasāvako buddho,",
"thi-ap17:179.4": "pūjatthaṃ yāti mātuyā.",
"thi-ap17:180.1": "Buddhassa parinibbānaṃ,",
"thi-ap17:180.2": "nedisaṃ āsi yādisaṃ;",
"thi-ap17:180.3": "Gotamīparinibbānaṃ,",
"thi-ap17:180.4": "atevacchariyaṃ ahu.",
"thi-ap17:181.1": "Buddho buddhassa nibbāne,",
"thi-ap17:181.2": "nopaṭiyādi bhikkhavo;",
"thi-ap17:181.3": "Buddho gotaminibbāne,",
"thi-ap17:181.4": "sāriputtādikā tathā.",
"thi-ap17:182.1": "Citakāni karitvāna,",
"thi-ap17:182.2": "sabbagandhamayāni te;",
"thi-ap17:182.3": "Gandhacuṇṇapakiṇṇāni,",
"thi-ap17:182.4": "jhāpayiṃsu ca tā tahiṃ.",
"thi-ap17:183.1": "Sesabhāgāni ḍayhiṃsu,",
"thi-ap17:183.2": "aṭṭhī sesāni sabbaso;",
"thi-ap17:183.3": "Ānando ca tadāvoca,",
"thi-ap17:183.4": "saṃvegajanakaṃ vaco.",
"thi-ap17:184.1": "“Gotamī nidhanaṃ yātā,",
"thi-ap17:184.2": "ḍayhañcassā sarīrakaṃ;",
"thi-ap17:184.3": "Saṅketaṃ buddhanibbānaṃ,",
"thi-ap17:184.4": "na cirena bhavissati”.",
"thi-ap17:185.1": "Tato gotamidhātūni,",
"thi-ap17:185.2": "tassā pattagatāni so;",
"thi-ap17:185.3": "Upanāmesi nāthassa,",
"thi-ap17:185.4": "ānando buddhacodito.",
"thi-ap17:186.1": "Pāṇinā tāni paggayha,",
"thi-ap17:186.2": "avoca isisattamo;",
"thi-ap17:186.3": "“Mahato sāravantassa,",
"thi-ap17:186.4": "yathā rukkhassa tiṭṭhato.",
"thi-ap17:187.1": "Yo so mahattaro khandho,",
"thi-ap17:187.2": "palujjeyya aniccatā;",
"thi-ap17:187.3": "Tathā bhikkhunisaṃghassa,",
"thi-ap17:187.4": "gotamī parinibbutā.",
"thi-ap17:188.1": "Aho acchariyaṃ mayhaṃ,",
"thi-ap17:188.2": "nibbutāyapi mātuyā;",
"thi-ap17:188.3": "Sarīramattasesāya,",
"thi-ap17:188.4": "natthi sokapariddavo.",
"thi-ap17:189.1": "Na sociyā paresaṃ sā,",
"thi-ap17:189.2": "tiṇṇasaṃsārasāgarā;",
"thi-ap17:189.3": "Parivajjitasantāpā,",
"thi-ap17:189.4": "sītibhūtā sunibbutā.",
"thi-ap17:190.1": "Paṇḍitāsi mahāpaññā,",
"thi-ap17:190.2": "puthupaññā tatheva ca;",
"thi-ap17:190.3": "Rattaññū bhikkhunīnaṃ sā,",
"thi-ap17:190.4": "evaṃ dhāretha bhikkhavo.",
"thi-ap17:191.1": "Iddhīsu ca vasī āsi,",
"thi-ap17:191.2": "dibbāya sotadhātuyā;",
"thi-ap17:191.3": "Cetopariyañāṇassa,",
"thi-ap17:191.4": "vasī āsi ca gotamī.",
"thi-ap17:192.1": "Pubbenivāsamaññāsi,",
"thi-ap17:192.2": "dibbacakkhu visodhitaṃ;",
"thi-ap17:192.3": "Sabbāsavā parikkhīṇā,",
"thi-ap17:192.4": "natthi tassā punabbhavo.",
"thi-ap17:193.1": "Atthadhammaniruttīsu,",
"thi-ap17:193.2": "paṭibhāne tatheva ca;",
"thi-ap17:193.3": "Parisuddhaṃ ahu ñāṇaṃ,",
"thi-ap17:193.4": "tasmā socaniyā na sā.",
"thi-ap17:194.1": "Ayoghanahatasseva,",
"thi-ap17:194.2": "jalato jātavedassa;",
"thi-ap17:194.3": "Anupubbūpasantassa,",
"thi-ap17:194.4": "yathā na ñāyate gati.",
"thi-ap17:195.1": "Evaṃ sammā vimuttānaṃ,",
"thi-ap17:195.2": "kāmabandhoghatārinaṃ;",
"thi-ap17:195.3": "Paññāpetuṃ gati natthi,",
"thi-ap17:195.4": "pattānaṃ acalaṃ sukhaṃ.",
"thi-ap17:196.1": "Attadīpā tato hotha,",
"thi-ap17:196.2": "satipaṭṭhānagocarā;",
"thi-ap17:196.3": "Bhāvetvā sattabojjhaṅge,",
"thi-ap17:196.4": "dukkhassantaṃ karissatha”.",
"thi-ap17:197.1": "Itthaṃ sudaṃ mahāpajāpatigotamī imā gāthāyo abhāsitthāti.",
"thi-ap17:198.1": "Mahāpajāpatigotamītheriyāpadānaṃ sattamaṃ."
}