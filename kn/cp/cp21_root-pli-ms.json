{
"cp21:1.1": "Cariyāpiṭaka",
"cp21:2.1": "Yudhañjayavagga",
"cp21:3.1": "Nekkhammapāramī 1",
"cp21:4.1": "Yudhañjayacariya",
"cp21:5.1": "“Yadāhaṃ amitayaso,",
"cp21:5.2": "Rājaputto yudhañjayo;",
"cp21:5.3": "Ussāvabinduṃ sūriyātape,",
"cp21:5.4": "Patitaṃ disvāna saṃvijiṃ.",
"cp21:6.1": "Taññevādhipatiṃ katvā,",
"cp21:6.2": "saṃvegamanubrūhayiṃ;",
"cp21:6.3": "Mātāpitū ca vanditvā,",
"cp21:6.4": "pabbajjamanuyācahaṃ.",
"cp21:7.1": "Yācanti maṃ pañjalikā,",
"cp21:7.2": "sanegamā saraṭṭhakā;",
"cp21:7.3": "‘Ajjeva putta paṭipajja,",
"cp21:7.4": "iddhaṃ phītaṃ mahāmahiṃ’.",
"cp21:8.1": "Sarājake sahorodhe,",
"cp21:8.2": "sanegame saraṭṭhake;",
"cp21:8.3": "Karuṇaṃ paridevante,",
"cp21:8.4": "anapekkhova pariccajiṃ.",
"cp21:9.1": "Kevalaṃ pathaviṃ rajjaṃ,",
"cp21:9.2": "ñātiparijanaṃ yasaṃ;",
"cp21:9.3": "Cajamāno na cintesiṃ,",
"cp21:9.4": "bodhiyāyeva kāraṇā.",
"cp21:10.1": "Mātāpitā na me dessā,",
"cp21:10.2": "napi me dessaṃ mahāyasaṃ;",
"cp21:10.3": "Sabbaññutaṃ piyaṃ mayhaṃ,",
"cp21:10.4": "tasmā rajjaṃ pariccajin”ti.",
"cp21:11.1": "Yudhañjayacariyaṃ paṭhamaṃ."
}