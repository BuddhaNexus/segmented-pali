{
"cp5:1.1": "Cariyāpiṭaka",
"cp5:2.1": "Akittivagga",
"cp5:3.1": "Dānapāramī 5",
"cp5:4.1": "Mahāgovindacariya",
"cp5:5.1": "“Punāparaṃ yadā homi,",
"cp5:5.2": "sattarājapurohito;",
"cp5:5.3": "Pūjito naradevehi,",
"cp5:5.4": "mahāgovindabrāhmaṇo.",
"cp5:6.1": "Tadāhaṃ sattarajjesu,",
"cp5:6.2": "Yaṃ me āsi upāyanaṃ;",
"cp5:6.3": "Tena demi mahādānaṃ,",
"cp5:6.4": "Akkhobbhaṃ sāgarūpamaṃ.",
"cp5:7.1": "Na me dessaṃ dhanaṃ dhaññaṃ,",
"cp5:7.2": "Napi natthi nicayo mayi;",
"cp5:7.3": "Sabbaññutaṃ piyaṃ mayhaṃ,",
"cp5:7.4": "Tasmā demi varaṃ dhanan”ti.",
"cp5:8.1": "Mahāgovindacariyaṃ pañcamaṃ."
}