{
"vv72:1.1": "Vimānavatthu",
"vv72:2.1": "Purisavimāna",
"vv72:3.1": "Pāyāsivagga",
"vv72:4.1": "8. Paṭhamakuṇḍalīvimānavatthu",
"vv72:5.1": "“Alaṅkato malyadharo suvattho,",
"vv72:5.2": "Sukuṇḍalī kappitakesamassu;",
"vv72:5.3": "Āmuttahatthābharaṇo yasassī,",
"vv72:5.4": "Dibbe vimānamhi yathāpi candimā.",
"vv72:6.1": "Dibbā ca vīṇā pavadanti vagguṃ,",
"vv72:6.2": "Aṭṭhaṭṭhakā sikkhitā sādhurūpā;",
"vv72:6.3": "Dibbā ca kaññā tidasacarā uḷārā,",
"vv72:6.4": "Naccanti gāyanti pamodayanti.",
"vv72:7.1": "Deviddhipattosi mahānubhāvo,",
"vv72:7.2": "Manussabhūto kimakāsi puññaṃ;",
"vv72:7.3": "Kenāsi evaṃ jalitānubhāvo,",
"vv72:7.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
"vv72:8.1": "So devaputto attamano, … pe …",
"vv72:8.4": "yassa kammassidaṃ phalaṃ.",
"vv72:9.1": "“Ahaṃ manussesu manussabhūto,",
"vv72:9.2": "Disvāna samaṇe sīlavante;",
"vv72:9.3": "Sampannavijjācaraṇe yasassī,",
"vv72:9.4": "Bahussute taṇhakkhayūpapanne;",
"vv72:9.5": "Annañca pānañca pasannacitto,",
"vv72:9.6": "Sakkacca dānaṃ vipulaṃ adāsiṃ.",
"vv72:10.1": "Tena metādiso vaṇṇo, … pe …",
"vv72:10.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
"vv72:11.1": "Paṭhamakuṇḍalīvimānaṃ aṭṭhamaṃ."
}