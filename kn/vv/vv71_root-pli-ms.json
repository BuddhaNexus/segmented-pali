{
"vv71:1.1": "Vimānavatthu",
"vv71:2.1": "Purisavimāna",
"vv71:3.1": "Pāyāsivagga",
"vv71:4.1": "7. Yavapālakavimānavatthu",
"vv71:5.1": "“Uccamidaṃ maṇithūṇaṃ vimānaṃ, … pe …",
"vv71:5.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
"vv71:6.1": "So devaputto attamano, … pe …",
"vv71:6.4": "yassa kammassidaṃ phalaṃ.",
"vv71:7.1": "“Ahaṃ manussesu manussabhūto,",
"vv71:7.2": "Ahosiṃ yavapālako;",
"vv71:7.3": "Addasaṃ virajaṃ bhikkhuṃ,",
"vv71:7.4": "Vippasannamanāvilaṃ.",
"vv71:8.1": "Tassa adāsahaṃ bhāgaṃ,",
"vv71:8.2": "pasanno sehi pāṇibhi;",
"vv71:8.3": "Kummāsapiṇḍaṃ datvāna,",
"vv71:8.4": "modāmi nandane vane.",
"vv71:9.1": "Tena metādiso vaṇṇo, … pe …",
"vv71:9.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
"vv71:10.1": "Yavapālakavimānaṃ sattamaṃ."
}