{
"vv55:1.1": "Vimānavatthu",
"vv55:2.1": "Purisavimāna",
"vv55:3.1": "Mahārathavagga",
"vv55:4.1": "5. Dvārapālavimānavatthu",
"vv55:5.1": "“Uccamidaṃ maṇithūṇaṃ vimānaṃ,",
"vv55:5.2": "Samantato dvādasa yojanāni;",
"vv55:5.3": "Kūṭāgārā sattasatā uḷārā,",
"vv55:5.4": "Veḷuriyathambhā rucakatthatā subhā.",
"vv55:6.1": "Tatthacchasi pivasi khādasi ca,",
"vv55:6.2": "Dibbā ca vīṇā pavadanti vagguṃ;",
"vv55:6.3": "Dibbā rasā kāmaguṇettha pañca,",
"vv55:6.4": "Nāriyo ca naccanti suvaṇṇachannā.",
"vv55:7.1": "Kena tetādiso vaṇṇo, … pe …",
"vv55:7.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
"vv55:8.1": "So devaputto attamano,",
"vv55:8.2": "moggallānena pucchito;",
"vv55:8.3": "Pañhaṃ puṭṭho viyākāsi,",
"vv55:8.4": "yassa kammassidaṃ phalaṃ.",
"vv55:9.1": "“Dibbaṃ mamaṃ vassasahassamāyu,",
"vv55:9.2": "Vācābhigītaṃ manasā pavattitaṃ;",
"vv55:9.3": "Ettāvatā ṭhassati puññakammo,",
"vv55:9.4": "Dibbehi kāmehi samaṅgibhūto.",
"vv55:10.1": "Tena metādiso vaṇṇo, … pe …",
"vv55:10.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
"vv55:11.1": "Dvārapālavimānaṃ pañcamaṃ."
}