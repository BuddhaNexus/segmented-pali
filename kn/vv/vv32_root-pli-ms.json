{
"vv32:1.1": "Vimānavatthu",
"vv32:2.1": "Itthivimāna",
"vv32:3.1": "Pāricchattakavagga",
"vv32:4.1": "4. Latāvimānavatthu",
"vv32:5.1": "Latā ca sajjā pavarā ca devatā,",
"vv32:5.2": "Accimatī rājavarassa sirīmato;",
"vv32:5.3": "Sutā ca rañño vessavaṇassa dhītā,",
"vv32:5.4": "Rājīmatī dhammaguṇehi sobhatha.",
"vv32:6.1": "Pañcettha nāriyo āgamaṃsu nhāyituṃ,",
"vv32:6.2": "Sītodakaṃ uppaliniṃ sivaṃ nadiṃ;",
"vv32:6.3": "Tā tattha nhāyitvā rametvā devatā,",
"vv32:6.4": "Naccitvā gāyitvā sutā lataṃ bravi.",
"vv32:7.1": "“Pucchāmi taṃ uppalamāladhārini,",
"vv32:7.2": "Āveḷini kañcanasannibhattace;",
"vv32:7.3": "Timiratambakkhi nabheva sobhane,",
"vv32:7.4": "Dīghāyukī kena kato yaso tava.",
"vv32:8.1": "Kenāsi bhadde patino piyatarā,",
"vv32:8.2": "Visiṭṭhakalyāṇitarassu rūpato;",
"vv32:8.3": "Padakkhiṇā naccanagītavādite,",
"vv32:8.4": "Ācikkha no tvaṃ naranāripucchitā”ti.",
"vv32:9.1": "“Ahaṃ manussesu manussabhūtā,",
"vv32:9.2": "Uḷārabhoge kule suṇisā ahosiṃ;",
"vv32:9.3": "Akkodhanā bhattuvasānuvattinī,",
"vv32:9.4": "Uposathe appamattā ahosiṃ.",
"vv32:10.1": "Manussabhūtā daharā apāpikā,",
"vv32:10.2": "Pasannacittā patimābhirādhayiṃ;",
"vv32:10.3": "Sadevaraṃ sassasuraṃ sadāsakaṃ,",
"vv32:10.4": "Abhirādhayiṃ tamhi kato yaso mama.",
"vv32:11.1": "Sāhaṃ tena kusalena kammunā,",
"vv32:11.2": "Catubbhi ṭhānehi visesamajjhagā;",
"vv32:11.3": "Āyuñca vaṇṇañca sukhaṃ balañca,",
"vv32:11.4": "Khiḍḍāratiṃ paccanubhomanappakaṃ”.",
"vv32:12.1": "“Sutaṃ nu taṃ bhāsati yaṃ ayaṃ latā,",
"vv32:12.2": "Yaṃ no apucchimha akittayī no;",
"vv32:12.3": "Patino kiramhākaṃ visiṭṭha nārīnaṃ,",
"vv32:12.4": "Gatī ca tāsaṃ pavarā ca devatā.",
"vv32:13.1": "Patīsu dhammaṃ pacarāma sabbā,",
"vv32:13.2": "Patibbatā yattha bhavanti itthiyo;",
"vv32:13.3": "Patīsu dhammaṃ pacaritva sabbā,",
"vv32:13.4": "Lacchāmase bhāsati yaṃ ayaṃ latā.",
"vv32:14.1": "Sīho yathā pabbatasānugocaro,",
"vv32:14.2": "Mahindharaṃ pabbatamāvasitvā;",
"vv32:14.3": "Pasayha hantvā itare catuppade,",
"vv32:14.4": "Khudde mige khādati maṃsabhojano.",
"vv32:15.1": "Tatheva saddhā idha ariyasāvikā,",
"vv32:15.2": "Bhattāraṃ nissāya patiṃ anubbatā;",
"vv32:15.3": "Kodhaṃ vadhitvā abhibhuyya maccharaṃ,",
"vv32:15.4": "Saggamhi sā modati dhammacārinī”ti.",
"vv32:16.1": "Latāvimānaṃ catutthaṃ."
}