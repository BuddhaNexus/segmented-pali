{
"vv46:1.1": "Vimānavatthu",
"vv46:2.1": "Itthivimāna",
"vv46:3.1": "Mañjiṭṭhakavagga",
"vv46:4.1": "8. Ambavimānavatthu",
"vv46:5.1": "“Dibbaṃ te ambavanaṃ rammaṃ,",
"vv46:5.2": "Pāsādettha mahallako;",
"vv46:5.3": "Nānātūriyasaṅghuṭṭho,",
"vv46:5.4": "Accharāgaṇaghosito.",
"vv46:6.1": "Padīpo cettha jalati,",
"vv46:6.2": "niccaṃ sovaṇṇayo mahā;",
"vv46:6.3": "Dussaphalehi rukkhehi,",
"vv46:6.4": "samantā parivārito.",
"vv46:7.1": "Kena tetādiso vaṇṇo, … pe …",
"vv46:7.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
"vv46:8.1": "Sā devatā attamanā, … pe …",
"vv46:8.4": "yassa kammassidaṃ phalaṃ.",
"vv46:9.1": "“Ahaṃ manussesu manussabhūtā,",
"vv46:9.2": "Purimāya jātiyā manussaloke;",
"vv46:9.3": "Vihāraṃ saṃghassa kāresiṃ,",
"vv46:9.4": "Ambehi parivāritaṃ.",
"vv46:10.1": "Pariyosite vihāre,",
"vv46:10.2": "kārente niṭṭhite mahe;",
"vv46:10.3": "Ambehi chādayitvāna,",
"vv46:10.4": "katvā dussamaye phale.",
"vv46:11.1": "Padīpaṃ tattha jāletvā,",
"vv46:11.2": "bhojayitvā gaṇuttamaṃ;",
"vv46:11.3": "Niyyādesiṃ taṃ saṃghassa,",
"vv46:11.4": "pasannā sehi pāṇibhi.",
"vv46:12.1": "Tena me ambavanaṃ rammaṃ,",
"vv46:12.2": "pāsādettha mahallako;",
"vv46:12.3": "Nānātūriyasaṅghuṭṭho,",
"vv46:12.4": "accharāgaṇaghosito.",
"vv46:13.1": "Padīpo cettha jalati,",
"vv46:13.2": "niccaṃ sovaṇṇayo mahā;",
"vv46:13.3": "Dussaphalehi rukkhehi,",
"vv46:13.4": "samantā parivārito.",
"vv46:14.1": "Tena metādiso vaṇṇo, … pe …",
"vv46:14.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
"vv46:15.1": "Ambavimānaṃ aṭṭhamaṃ."
}