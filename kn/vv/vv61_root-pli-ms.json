{
"vv61:1.1": "Vimānavatthu",
"vv61:2.1": "Purisavimāna",
"vv61:3.1": "Mahārathavagga",
"vv61:4.1": "11. Dutiyanāgavimānavatthu",
"vv61:5.1": "“Mahantaṃ nāgaṃ abhiruyha,",
"vv61:5.2": "Sabbasetaṃ gajuttamaṃ;",
"vv61:5.3": "Vanā vanaṃ anupariyāsi,",
"vv61:5.4": "Nārīgaṇapurakkhato;",
"vv61:5.5": "Obhāsento disā sabbā,",
"vv61:5.6": "Osadhī viya tārakā.",
"vv61:6.1": "Kena tetādiso vaṇṇo, … pe …",
"vv61:6.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
"vv61:7.1": "So devaputto attamano,",
"vv61:7.2": "vaṅgīseneva pucchito;",
"vv61:7.3": "Pañhaṃ puṭṭho viyākāsi,",
"vv61:7.4": "yassa kammassidaṃ phalaṃ.",
"vv61:8.1": "“Ahaṃ manussesu manussabhūto,",
"vv61:8.2": "Upāsako cakkhumato ahosiṃ;",
"vv61:8.3": "Pāṇātipātā virato ahosiṃ,",
"vv61:8.4": "Loke adinnaṃ parivajjayissaṃ.",
"vv61:9.1": "Amajjapo no ca musā abhāṇiṃ,",
"vv61:9.2": "Sakena dārena ca tuṭṭho ahosiṃ;",
"vv61:9.3": "Annañca pānañca pasannacitto,",
"vv61:9.4": "Sakkacca dānaṃ vipulaṃ adāsiṃ.",
"vv61:10.1": "Tena metādiso vaṇṇo, … pe …",
"vv61:10.4": "vaṇṇo ca me sabbadisā pabhāsatī”ti.",
"vv61:11.1": "Dutiyanāgavimānaṃ ekādasamaṃ."
}