{
"vv27:1.1": "Vimānavatthu",
"vv27:2.1": "Itthivimāna",
"vv27:3.1": "Cittalatāvagga",
"vv27:4.1": "10. Paṭhamabhikkhādāyikāvimānavatthu",
"vv27:5.1": "“Abhikkantena vaṇṇena,",
"vv27:5.2": "yā tvaṃ tiṭṭhasi devate;",
"vv27:5.3": "Obhāsentī disā sabbā,",
"vv27:5.4": "osadhī viya tārakā.",
"vv27:6.1": "Kena tetādiso vaṇṇo, … pe …",
"vv27:6.4": "vaṇṇo ca te sabbadisā pabhāsatī”ti.",
"vv27:7.1": "Sā devatā attamanā, … pe …",
"vv27:7.4": "yassa kammassidaṃ phalaṃ.",
"vv27:8.1": "“Ahaṃ manussesu manussabhūtā,",
"vv27:8.2": "Purimāya jātiyā manussaloke.",
"vv27:9.1": "Addasaṃ virajaṃ buddhaṃ,",
"vv27:9.2": "vippasannamanāvilaṃ;",
"vv27:9.3": "Tassa adāsahaṃ bhikkhaṃ,",
"vv27:9.4": "pasannā sehi pāṇibhi.",
"vv27:10.1": "Tena metādiso vaṇṇo, … pe …",
"vv27:10.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
"vv27:11.1": "Paṭhamabhikkhādāyikāvimānaṃ dasamaṃ."
}