{
"vv74:1.1": "Vimānavatthu",
"vv74:2.1": "Purisavimāna",
"vv74:3.1": "Pāyāsivagga",
"vv74:4.1": "10. (Uttara) Pāyāsivimānavatthu",
"vv74:5.1": "“Yā devarājassa sabhā sudhammā,",
"vv74:5.2": "Yatthacchati devasaṅgho samaggo;",
"vv74:5.3": "Tathūpamaṃ tuyhamidaṃ vimānaṃ,",
"vv74:5.4": "Obhāsayaṃ tiṭṭhati antalikkhe.",
"vv74:6.1": "Deviddhipattosi mahānubhāvo,",
"vv74:6.2": "Manussabhūto kimakāsi puññaṃ;",
"vv74:6.3": "Kenāsi evaṃ jalitānubhāvo,",
"vv74:6.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
"vv74:7.1": "So devaputto attamano, … pe …",
"vv74:7.4": "yassa kammassidaṃ phalaṃ.",
"vv74:8.1": "“Ahaṃ manussesu manussabhūto,",
"vv74:8.2": "Rañño pāyāsissa ahosiṃ māṇavo;",
"vv74:8.3": "Laddhā dhanaṃ saṃvibhāgaṃ akāsiṃ,",
"vv74:8.4": "Piyā ca me sīlavanto ahesuṃ;",
"vv74:8.5": "Annañca pānañca pasannacitto,",
"vv74:8.6": "Sakkacca dānaṃ vipulaṃ adāsiṃ.",
"vv74:9.1": "Tena metādiso vaṇṇo, … pe …",
"vv74:9.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
"vv74:10.1": "Pāyāsivimānaṃ dasamaṃ.",
"vv74:11.1": "Pāyāsivaggo chaṭṭho.",
"vv74:12.1": "Tassuddānaṃ",
"vv74:13.1": "Dve agārino phaladāyī,",
"vv74:13.2": "Dve upassayadāyī bhikkhāya dāyī;",
"vv74:13.3": "Yavapālako ceva dve,",
"vv74:13.4": "Kuṇḍalino pāyāsīti.",
"vv74:14.1": "Purisānaṃ dutiyo vaggo pavuccatīti."
}