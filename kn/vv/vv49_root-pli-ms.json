{
"vv49:1.1": "Vimānavatthu",
"vv49:2.1": "Itthivimāna",
"vv49:3.1": "Mañjiṭṭhakavagga",
"vv49:4.1": "11. Vandanavimānavatthu",
"vv49:5.1": "“Abhikkantena vaṇṇena,",
"vv49:5.2": "yā tvaṃ tiṭṭhasi devate;",
"vv49:5.3": "Obhāsentī disā sabbā,",
"vv49:5.4": "osadhī viya tārakā.",
"vv49:6.1": "Kena tetādiso vaṇṇo, … pe …",
"vv49:6.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
"vv49:7.1": "Sā devatā attamanā, … pe …",
"vv49:7.4": "yassa kammassidaṃ phalaṃ.",
"vv49:8.1": "“Ahaṃ manussesu manussabhūtā,",
"vv49:8.2": "Disvāna samaṇe sīlavante;",
"vv49:8.3": "Pādāni vanditvā manaṃ pasādayiṃ,",
"vv49:8.4": "Vittā cahaṃ añjalikaṃ akāsiṃ.",
"vv49:9.1": "Tena metādiso vaṇṇo, … pe …",
"vv49:9.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
"vv49:10.1": "Vandanavimānaṃ ekādasamaṃ."
}