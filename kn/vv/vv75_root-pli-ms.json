{
"vv75:1.1": "Vimānavatthu",
"vv75:2.1": "Purisavimāna",
"vv75:3.1": "Sunikkhittavagga",
"vv75:4.1": "1. Cittalatāvimānavatthu",
"vv75:5.1": "“Yathā vanaṃ cittalataṃ pabhāsati,",
"vv75:5.2": "Uyyānaseṭṭhaṃ tidasānamuttamaṃ;",
"vv75:5.3": "Tathūpamaṃ tuyhamidaṃ vimānaṃ,",
"vv75:5.4": "Obhāsayaṃ tiṭṭhati antalikkhe.",
"vv75:6.1": "Deviddhipattosi mahānubhāvo,",
"vv75:6.2": "Manussabhūto kimakāsi puññaṃ;",
"vv75:6.3": "Kenāsi evaṃ jalitānubhāvo,",
"vv75:6.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
"vv75:7.1": "So devaputto attamano, … pe …",
"vv75:7.4": "yassa kammassidaṃ phalaṃ.",
"vv75:8.1": "“Ahaṃ manussesu manussabhūto,",
"vv75:8.2": "Daliddo atāṇo kapaṇo kammakaro ahosiṃ;",
"vv75:8.3": "Jiṇṇe ca mātāpitaro abhāriṃ,",
"vv75:8.4": "Piyā ca me sīlavanto ahesuṃ;",
"vv75:8.5": "Annañca pānañca pasannacitto,",
"vv75:8.6": "Sakkacca dānaṃ vipulaṃ adāsiṃ.",
"vv75:9.1": "Tena metādiso vaṇṇo, … pe …",
"vv75:9.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
"vv75:10.1": "Cittalatāvimānaṃ paṭhamaṃ."
}