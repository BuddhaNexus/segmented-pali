{
"vv58:1.1": "Vimānavatthu",
"vv58:2.1": "Purisavimāna",
"vv58:3.1": "Mahārathavagga",
"vv58:4.1": "8. Paṭhamasūcivimānavatthu",
"vv58:5.1": "“Uccamidaṃ maṇithūṇaṃ vimānaṃ,",
"vv58:5.2": "Samantato dvādasa yojanāni;",
"vv58:5.3": "Kūṭāgārā sattasatā uḷārā,",
"vv58:5.4": "Veḷuriyathambhā rucakatthatā subhā.",
"vv58:6.1": "Tatthacchasi pivasi khādasi ca,",
"vv58:6.2": "Dibbā ca vīṇā pavadanti vagguṃ;",
"vv58:6.3": "Dibbā rasā kāmaguṇettha pañca,",
"vv58:6.4": "Nāriyo ca naccanti suvaṇṇachannā.",
"vv58:7.1": "Kena tetādiso vaṇṇo, … pe …",
"vv58:7.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
"vv58:8.1": "So devaputto attamano, … pe …",
"vv58:8.4": "yassa kammassidaṃ phalaṃ.",
"vv58:9.1": "“Yaṃ dadāti na taṃ hoti,",
"vv58:9.2": "Yañceva dajjā tañceva seyyo;",
"vv58:9.3": "Sūci dinnā sūcimeva seyyo.",
"vv58:10.1": "Tena metādiso vaṇṇo, … pe …",
"vv58:10.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
"vv58:11.1": "Paṭhamasūcivimānaṃ aṭṭhamaṃ."
}