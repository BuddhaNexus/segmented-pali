{
"vv64:1.1": "Vimānavatthu",
"vv64:2.1": "Purisavimāna",
"vv64:3.1": "Mahārathavagga",
"vv64:4.1": "14. Mahārathavimānavatthu",
"vv64:5.1": "“Sahassayuttaṃ hayavāhanaṃ subhaṃ,",
"vv64:5.2": "Āruyhimaṃ sandanaṃ nekacittaṃ;",
"vv64:5.3": "Uyyānabhūmiṃ abhito anukkamaṃ,",
"vv64:5.4": "Purindado bhūtapatīva vāsavo.",
"vv64:6.1": "Sovaṇṇamayā te rathakubbarā ubho,",
"vv64:6.2": "Phalehi aṃsehi atīva saṅgatā;",
"vv64:6.3": "Sujātagumbā naravīraniṭṭhitā,",
"vv64:6.4": "Virocatī pannaraseva cando.",
"vv64:7.1": "Suvaṇṇajālāvatato ratho ayaṃ,",
"vv64:7.2": "Bahūhi nānāratanehi cittito;",
"vv64:7.3": "Sunandighoso ca subhassaro ca,",
"vv64:7.4": "Virocatī cāmarahatthabāhubhi.",
"vv64:8.1": "Imā ca nābhyo manasābhinimmitā,",
"vv64:8.2": "Rathassa pādantaramajjhabhūsitā;",
"vv64:8.3": "Imā ca nābhyo satarājicittitā,",
"vv64:8.4": "Sateratā vijjurivappabhāsare.",
"vv64:9.1": "Anekacittāvatato ratho ayaṃ,",
"vv64:9.2": "Puthū ca nemī ca sahassaraṃsiko;",
"vv64:9.3": "Tesaṃ saro suyyati vaggurūpo,",
"vv64:9.4": "Pañcaṅgikaṃ tūriyamivappavāditaṃ.",
"vv64:10.1": "Sirasmiṃ cittaṃ maṇicandakappitaṃ,",
"vv64:10.2": "Sadā visuddhaṃ ruciraṃ pabhassaraṃ;",
"vv64:10.3": "Suvaṇṇarājīhi atīva saṅgataṃ,",
"vv64:10.4": "Veḷuriyarājīva atīva sobhati.",
"vv64:11.1": "Ime ca vāḷī maṇicandakappitā,",
"vv64:11.2": "Ārohakambū sujavā brahūpamā;",
"vv64:11.3": "Brahā mahantā balino mahājavā,",
"vv64:11.4": "Mano tavaññāya tatheva siṃsare.",
"vv64:12.1": "Ime ca sabbe sahitā catukkamā,",
"vv64:12.2": "Mano tavaññāya tatheva siṃsare;",
"vv64:12.3": "Samaṃ vahantī mudukā anuddhatā,",
"vv64:12.4": "Āmodamānā turagānamuttama.",
"vv64:13.1": "Dhunanti vagganti patanti cambare,",
"vv64:13.2": "Abbhuddhunantā sukate piḷandhane;",
"vv64:13.3": "Tesaṃ saro suyyati vaggurūpo,",
"vv64:13.4": "Pañcaṅgikaṃ tūriyamivappavāditaṃ.",
"vv64:14.1": "Rathassa ghoso apiḷandhanāna ca,",
"vv64:14.2": "Khurassa nādo abhihiṃsanāya ca;",
"vv64:14.3": "Ghoso suvaggū samitassa suyyati,",
"vv64:14.4": "Gandhabbatūriyāni vicitrasaṃvane.",
"vv64:15.1": "Rathe ṭhitā tā migamandalocanā,",
"vv64:15.2": "Āḷārapamhā hasitā piyaṃvadā;",
"vv64:15.3": "Veḷuriyajālāvatatā tanucchavā,",
"vv64:15.4": "Sadeva gandhabbasūraggapūjitā.",
"vv64:16.1": "Tā rattarattambarapītavāsasā,",
"vv64:16.2": "Visālanettā abhirattalocanā;",
"vv64:16.3": "Kule sujātā sutanū sucimhitā,",
"vv64:16.4": "Rathe ṭhitā pañjalikā upaṭṭhitā.",
"vv64:17.1": "Tā kambukeyūradharā suvāsasā,",
"vv64:17.2": "Sumajjhimā ūruthanūpapannā;",
"vv64:17.3": "Vaṭṭaṅguliyo sumukhā sudassanā,",
"vv64:17.4": "Rathe ṭhitā pañjalikā upaṭṭhitā.",
"vv64:18.1": "Aññā suveṇī susu missakesiyo,",
"vv64:18.2": "Samaṃ vibhattāhi pabhassarāhi ca;",
"vv64:18.3": "Anubbatā tā tava mānase ratā,",
"vv64:18.4": "Rathe ṭhitā pañjalikā upaṭṭhitā.",
"vv64:19.1": "Āveḷiniyo padumuppalacchadā,",
"vv64:19.2": "Alaṅkatā candanasāravāsitā;",
"vv64:19.3": "Anubbatā tā tava mānase ratā,",
"vv64:19.4": "Rathe ṭhitā pañjalikā upaṭṭhitā.",
"vv64:20.1": "Tā māliniyo padumuppalacchadā,",
"vv64:20.2": "Alaṅkatā candanasāravāsitā;",
"vv64:20.3": "Anubbatā tā tava mānase ratā,",
"vv64:20.4": "Rathe ṭhitā pañjalikā upaṭṭhitā.",
"vv64:21.1": "Kaṇṭhesu te yāni piḷandhanāni,",
"vv64:21.2": "Hatthesu pādesu tatheva sīse;",
"vv64:21.3": "Obhāsayantī dasa sabbaso disā,",
"vv64:21.4": "Abbhuddayaṃ sāradikova bhāṇumā.",
"vv64:22.1": "Vātassa vegena ca sampakampitā,",
"vv64:22.2": "Bhujesu mālā apiḷandhanāni ca;",
"vv64:22.3": "Muñcanti ghosaṃ ruciraṃ suciṃ subhaṃ,",
"vv64:22.4": "Sabbehi viññūhi sutabbarūpaṃ.",
"vv64:23.1": "Uyyānabhūmyā ca duvaddhato ṭhitā,",
"vv64:23.2": "Rathā ca nāgā tūriyāni ca saro;",
"vv64:23.3": "Tameva devinda pamodayanti,",
"vv64:23.4": "Vīṇā yathā pokkharapattabāhubhi.",
"vv64:24.1": "Imāsu vīṇāsu bahūsu vaggūsu,",
"vv64:24.2": "Manuññarūpāsu hadayeritaṃ pītiṃ;",
"vv64:24.3": "Pavajjamānāsu atīva accharā,",
"vv64:24.4": "Bhamanti kaññā padumesu sikkhitā.",
"vv64:25.1": "Yadā ca gītāni ca vāditāni ca,",
"vv64:25.2": "Naccāni cimāni samenti ekato;",
"vv64:25.3": "Athettha naccanti athettha accharā,",
"vv64:25.4": "Obhāsayantī ubhato varitthiyo.",
"vv64:26.1": "So modasi tūriyagaṇappabodhano,",
"vv64:26.2": "Mahīyamāno vajirāvudhoriva;",
"vv64:26.3": "Imāsu vīṇāsu bahūsu vaggūsu,",
"vv64:26.4": "Manuññarūpāsu hadayeritaṃ pītiṃ.",
"vv64:27.1": "Kiṃ tvaṃ pure kammamakāsi attanā,",
"vv64:27.2": "Manussabhūto purimāya jātiyā;",
"vv64:27.3": "Uposathaṃ kaṃ vā tuvaṃ upāvasi,",
"vv64:27.4": "Kaṃ dhammacariyaṃ vatamābhirocayi.",
"vv64:28.1": "Nayīdamappassa katassa kammuno,",
"vv64:28.2": "Pubbe suciṇṇassa uposathassa vā;",
"vv64:28.3": "Iddhānubhāvo vipulo ayaṃ tava,",
"vv64:28.4": "Yaṃ devasaṅghaṃ abhirocase bhusaṃ.",
"vv64:29.1": "Dānassa te idaṃ phalaṃ,",
"vv64:29.2": "atho sīlassa vā pana;",
"vv64:29.3": "Atho añjalikammassa,",
"vv64:29.4": "taṃ me akkhāhi pucchito”ti.",
"vv64:30.1": "So devaputto attamano,",
"vv64:30.2": "moggallānena pucchito;",
"vv64:30.3": "Pañhaṃ puṭṭho viyākāsi,",
"vv64:30.4": "yassa kammassidaṃ phalanti.",
"vv64:31.1": "“Jitindriyaṃ buddhamanomanikkamaṃ,",
"vv64:31.2": "Naruttamaṃ kassapamaggapuggalaṃ;",
"vv64:31.3": "Avāpurantaṃ amatassa dvāraṃ,",
"vv64:31.4": "Devātidevaṃ satapuññalakkhaṇaṃ.",
"vv64:32.1": "Tamaddasaṃ kuñjaramoghatiṇṇaṃ,",
"vv64:32.2": "Suvaṇṇasiṅgīnadabimbasādisaṃ;",
"vv64:32.3": "Disvāna taṃ khippamahuṃ sucīmano,",
"vv64:32.4": "Tameva disvāna subhāsitaddhajaṃ.",
"vv64:33.1": "Tamannapānaṃ athavāpi cīvaraṃ,",
"vv64:33.2": "Suciṃ paṇītaṃ rasasā upetaṃ;",
"vv64:33.3": "Pupphābhikkiṇamhi sake nivesane,",
"vv64:33.4": "Patiṭṭhapesiṃ sa asaṅgamānaso.",
"vv64:34.1": "Tamannapānena ca cīvarena ca,",
"vv64:34.2": "Khajjena bhojjena ca sāyanena ca;",
"vv64:34.3": "Santappayitvā dvipadānamuttamaṃ,",
"vv64:34.4": "So saggaso devapure ramāmahaṃ.",
"vv64:35.1": "Etenupāyena imaṃ niraggaḷaṃ,",
"vv64:35.2": "Yaññaṃ yajitvā tividhaṃ visuddhaṃ;",
"vv64:35.3": "Pahāyahaṃ mānusakaṃ samussayaṃ,",
"vv64:35.4": "Indūpamo devapure ramāmahaṃ.",
"vv64:36.1": "Āyuñca vaṇṇañca sukhaṃ balañca,",
"vv64:36.2": "Paṇītarūpaṃ abhikaṅkhatā muni;",
"vv64:36.3": "Annañca pānañca bahuṃ susaṅkhataṃ,",
"vv64:36.4": "Patiṭṭhapetabbamasaṅgamānase.",
"vv64:37.1": "Nayimasmiṃ loke parasmiṃ vā pana,",
"vv64:37.2": "Buddhena seṭṭho va samo va vijjati;",
"vv64:37.3": "Āhuneyyānaṃ paramāhutiṃ gato,",
"vv64:37.4": "Puññatthikānaṃ vipulapphalesinan”ti.",
"vv64:38.1": "Mahārathavimānaṃ cuddasamaṃ.",
"vv64:38.2": "Mahārathavaggo pañcamo.",
"vv64:39.1": "Tassuddānaṃ",
"vv64:40.1": "Maṇḍūko revatī chatto,",
"vv64:40.2": "Kakkaṭo dvārapālako;",
"vv64:40.3": "Dve karaṇīyā dve sūci,",
"vv64:40.4": "Tayo nāgā ca dve rathā;",
"vv64:40.5": "Purisānaṃ paṭhamo vaggo pavuccatīti.",
"vv64:41.1": "Bhāṇavāraṃ tatiyaṃ."
}