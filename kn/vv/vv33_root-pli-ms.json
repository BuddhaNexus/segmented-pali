{
"vv33:1.1": "Vimānavatthu",
"vv33:2.1": "Itthivimāna",
"vv33:3.1": "Pāricchattakavagga",
"vv33:4.1": "Guttilavimānavatthu",
"vv33:5.1": "1. Vatthuttamadāyikāvimānavatthu",
"vv33:6.1": "“Sattatantiṃ sumadhuraṃ,",
"vv33:6.2": "Rāmaṇeyyaṃ avācayiṃ;",
"vv33:6.3": "So maṃ raṅgamhi avheti,",
"vv33:6.4": "‘Saraṇaṃ me hohi kosiyā’”ti.",
"vv33:7.1": "“Ahaṃ te saraṇaṃ homi,",
"vv33:7.2": "Ahamācariyapūjako;",
"vv33:7.3": "Na taṃ jayissati sisso,",
"vv33:7.4": "Sissamācariya jessasī”ti.",
"vv33:8.1": "“Abhikkantena vaṇṇena,",
"vv33:8.2": "yā tvaṃ tiṭṭhasi devate;",
"vv33:8.3": "Obhāsentī disā sabbā,",
"vv33:8.4": "osadhī viya tārakā.",
"vv33:9.1": "Kena tetādiso vaṇṇo,",
"vv33:9.2": "kena te idha mijjhati;",
"vv33:9.3": "Uppajjanti ca te bhogā,",
"vv33:9.4": "ye keci manaso piyā.",
"vv33:10.1": "Pucchāmi taṃ devi mahānubhāve,",
"vv33:10.2": "Manussabhūtā kimakāsi puññaṃ;",
"vv33:10.3": "Kenāsi evaṃ jalitānubhāvā,",
"vv33:10.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
"vv33:11.1": "Sā devatā attamanā,",
"vv33:11.2": "moggallānena pucchitā;",
"vv33:11.3": "Pañhaṃ puṭṭhā viyākāsi,",
"vv33:11.4": "yassa kammassidaṃ phalaṃ.",
"vv33:12.1": "“Vatthuttamadāyikā nārī,",
"vv33:12.2": "Pavarā hoti naresu nārīsu;",
"vv33:12.3": "Evaṃ piyarūpadāyikā manāpaṃ,",
"vv33:12.4": "Dibbaṃ sā labhate upecca ṭhānaṃ.",
"vv33:13.1": "Tassā me passa vimānaṃ,",
"vv33:13.2": "Accharā kāmavaṇṇinīhamasmi;",
"vv33:13.3": "Accharāsahassassāhaṃ,",
"vv33:13.4": "Pavarā passa puññānaṃ vipākaṃ.",
"vv33:14.1": "Tena metādiso vaṇṇo,",
"vv33:14.2": "tena me idha mijjhati;",
"vv33:14.3": "Uppajjanti ca me bhogā,",
"vv33:14.4": "ye keci manaso piyā.",
"vv33:15.1": "Akkhāmi te bhikkhu mahānubhāva,",
"vv33:15.2": "Manussabhūtā yamakāsi puññaṃ;",
"vv33:15.3": "Tenamhi evaṃ jalitānubhāvā,",
"vv33:15.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
"vv33:16.1": "(Anantaraṃ caturavimānaṃ yathā vatthadāyikāvimānaṃ tathā vitthāretabbaṃ.)",
