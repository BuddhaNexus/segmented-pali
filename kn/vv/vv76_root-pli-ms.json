{
"vv76:1.1": "Vimānavatthu",
"vv76:2.1": "Purisavimāna",
"vv76:3.1": "Sunikkhittavagga",
"vv76:4.1": "2. Nandanavimānavatthu",
"vv76:5.1": "“Yathā vanaṃ nandanaṃ pabhāsati,",
"vv76:5.2": "Uyyānaseṭṭhaṃ tidasānamuttamaṃ;",
"vv76:5.3": "Tathūpamaṃ tuyhamidaṃ vimānaṃ,",
"vv76:5.4": "Obhāsayaṃ tiṭṭhati antalikkhe.",
"vv76:6.1": "Deviddhipattosi mahānubhāvo,",
"vv76:6.2": "Manussabhūto kimakāsi puññaṃ;",
"vv76:6.3": "Kenāsi evaṃ jalitānubhāvo,",
"vv76:6.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
"vv76:7.1": "So devaputto attamano, … pe …",
"vv76:7.4": "yassa kammassidaṃ phalaṃ.",
"vv76:8.1": "“Ahaṃ manussesu manussabhūto,",
"vv76:8.2": "Daliddo atāṇo kapaṇo kammakaro ahosiṃ;",
"vv76:8.3": "Jiṇṇe ca mātāpitaro abhāriṃ,",
"vv76:8.4": "Piyā ca me sīlavanto ahesuṃ;",
"vv76:8.5": "Annañca pānañca pasannacitto,",
"vv76:8.6": "Sakkacca dānaṃ vipulaṃ adāsiṃ.",
"vv76:9.1": "Tena metādiso vaṇṇo, … pe …",
"vv76:9.4": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
"vv76:10.1": "Nandanavimānaṃ dutiyaṃ."
}