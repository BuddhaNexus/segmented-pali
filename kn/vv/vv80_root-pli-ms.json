{
"vv80:1.1": "Vimānavatthu",
"vv80:2.1": "Purisavimāna",
"vv80:3.1": "Sunikkhittavagga",
"vv80:4.1": "6. Gopālavimānavatthu",
"vv80:5.1": "“Disvāna devaṃ paṭipucchi bhikkhu,",
"vv80:5.2": "Ucce vimānamhi ciraṭṭhitike;",
"vv80:5.3": "Āmuttahatthābharaṇaṃ yasassiṃ,",
"vv80:5.4": "Dibbe vimānamhi yathāpi candimā.",
"vv80:6.1": "Alaṅkato malyadharo suvattho,",
"vv80:6.2": "Sukuṇḍalī kappitakesamassu;",
"vv80:6.3": "Āmuttahatthābharaṇo yasassī,",
"vv80:6.4": "Dibbe vimānamhi yathāpi candimā.",
"vv80:7.1": "Dibbā ca vīṇā pavadanti vagguṃ,",
"vv80:7.2": "Aṭṭhaṭṭhakā sikkhitā sādhurūpā;",
"vv80:7.3": "Dibbā ca kaññā tidasacarā uḷārā,",
"vv80:7.4": "Naccanti gāyanti pamodayanti.",
"vv80:8.1": "Deviddhipattosi mahānubhāvo,",
"vv80:8.2": "Manussabhūto kimakāsi puññaṃ;",
"vv80:8.3": "Kenāsi evaṃ jalitānubhāvo,",
"vv80:8.4": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
"vv80:9.1": "So devaputto attamano, … pe …",
"vv80:9.4": "yassa kammassidaṃ phalaṃ.",
"vv80:10.1": "“Ahaṃ manussesu manussabhūto,",
"vv80:10.2": "Saṅgamma rakkhissaṃ paresaṃ dhenuyo;",
"vv80:10.3": "Tato ca āgā samaṇo mamantike,",
"vv80:10.4": "Gāvo ca māse agamaṃsu khādituṃ.",
"vv80:11.1": "Dvayajja kiccaṃ ubhayañca kāriyaṃ,",
"vv80:11.2": "Iccevahaṃ bhante tadā vicintayiṃ;",
"vv80:11.3": "Tato ca saññaṃ paṭiladdha yoniso,",
"vv80:11.4": "Dadāmi bhanteti khipiṃ anantakaṃ.",
"vv80:12.1": "So māsakhettaṃ turito avāsariṃ,",
"vv80:12.2": "Purā ayaṃ bhañjati yassidaṃ dhanaṃ;",
"vv80:12.3": "Tato ca kaṇho urago mahāviso,",
"vv80:12.4": "Aḍaṃsi pāde turitassa me sato.",
"vv80:13.1": "Svāhaṃ aṭṭomhi dukkhena pīḷito,",
"vv80:13.2": "Bhikkhu ca taṃ sāmaṃ muñcitvānantakaṃ;",
"vv80:13.3": "Ahāsi kummāsaṃ mamānukampayā,",
"vv80:13.4": "Tato cuto kālakatomhi devatā.",
"vv80:14.1": "Tadeva kammaṃ kusalaṃ kataṃ mayā,",
"vv80:14.2": "Sukhañca kammaṃ anubhomi attanā;",
"vv80:14.3": "Tayā hi bhante anukampito bhusaṃ,",
"vv80:14.4": "Kataññutāya abhivādayāmi taṃ.",
"vv80:15.1": "Sadevake loke samārake ca,",
"vv80:15.2": "Añño muni natthi tayānukampako;",
"vv80:15.3": "Tayā hi bhante anukampito bhusaṃ,",
"vv80:15.4": "Kataññutāya abhivādayāmi taṃ.",
"vv80:16.1": "Imasmiṃ loke parasmiṃ vā pana,",
"vv80:16.2": "Añño munī natthi tayānukampako;",
"vv80:16.3": "Tayā hi bhante anukampito bhusaṃ,",
"vv80:16.4": "Kataññutāya abhivādayāmi tan”ti.",
"vv80:17.1": "Gopālavimānaṃ chaṭṭhaṃ."
}