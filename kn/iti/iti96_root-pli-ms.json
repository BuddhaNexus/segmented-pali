{
"iti96:1.1": "Itivuttaka 96",
"iti96:2.1": "Kāmayogasutta",
"iti96:3.1": "Vuttañhetaṃ bhagavatā vuttamarahatāti me sutaṃ:",
"iti96:4.1": "“Kāmayogayutto, bhikkhave, bhavayogayutto āgāmī hoti āgantā itthattaṃ.",
"iti96:4.2": "Kāmayogavisaṃyutto, bhikkhave, bhavayogayutto anāgāmī hoti anāgantā itthattaṃ.",
"iti96:4.3": "Kāmayogavisaṃyutto, bhikkhave, bhavayogavisaṃyutto arahā hoti, khīṇāsavo”ti.",
"iti96:4.4": "Etamatthaṃ bhagavā avoca.",
"iti96:4.5": "Tatthetaṃ iti vuccati:",
"iti96:5.1": "“Kāmayogena saṃyuttā,",
"iti96:5.2": "bhavayogena cūbhayaṃ;",
"iti96:5.3": "Sattā gacchanti saṃsāraṃ,",
"iti96:5.4": "jātimaraṇagāmino.",
"iti96:6.1": "Ye ca kāme pahantvāna,",
"iti96:6.2": "appattā āsavakkhayaṃ;",
"iti96:6.3": "Bhavayogena saṃyuttā,",
"iti96:6.4": "anāgāmīti vuccare.",
"iti96:7.1": "Ye ca kho chinnasaṃsayā,",
"iti96:7.2": "khīṇamānapunabbhavā;",
"iti96:7.3": "Te ve pāraṅgatā loke,",
"iti96:7.4": "ye pattā āsavakkhayan”ti.",
"iti96:8.1": "Ayampi attho vutto bhagavatā, iti me sutanti.",
"iti96:9.1": "Sattamaṃ.",
"iti96:10.1": "Tatiyabhāṇavāraṃ."
}