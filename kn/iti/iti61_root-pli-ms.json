{
"iti61:1.1": "Itivuttaka 61",
"iti61:2.1": "Cakkhusutta",
"iti61:3.1": "Vuttañhetaṃ bhagavatā vuttamarahatāti me sutaṃ:",
"iti61:4.1": "“Tīṇimāni, bhikkhave, cakkhūni.",
"iti61:4.2": "Katamāni tīṇi?",
"iti61:4.3": "Maṃsacakkhu, dibbacakkhu, paññācakkhu—",
"iti61:4.4": "imāni kho, bhikkhave, tīṇi cakkhūnī”ti.",
"iti61:4.5": "Etamatthaṃ bhagavā avoca.",
"iti61:4.6": "Tatthetaṃ iti vuccati:",
"iti61:5.1": "“Maṃsacakkhu dibbacakkhu,",
"iti61:5.2": "paññācakkhu anuttaraṃ;",
"iti61:5.3": "Etāni tīṇi cakkhūni,",
"iti61:5.4": "akkhāsi purisuttamo.",
"iti61:6.1": "Maṃsacakkhussa uppādo,",
"iti61:6.2": "maggo dibbassa cakkhuno;",
"iti61:6.3": "Yato ñāṇaṃ udapādi,",
"iti61:6.4": "paññācakkhu anuttaraṃ;",
"iti61:6.5": "Yassa cakkhussa paṭilābhā,",
"iti61:6.6": "sabbadukkhā pamuccatī”ti.",
"iti61:7.1": "Ayampi attho vutto bhagavatā, iti me sutanti.",
"iti61:8.1": "Dutiyaṃ."
}