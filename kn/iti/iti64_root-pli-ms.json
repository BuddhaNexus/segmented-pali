{
"iti64:1.1": "Itivuttaka 64",
"iti64:2.1": "Duccaritasutta",
"iti64:3.1": "Vuttañhetaṃ bhagavatā vuttamarahatāti me sutaṃ:",
"iti64:4.1": "“Tīṇimāni, bhikkhave, duccaritāni.",
"iti64:4.2": "Katamāni tīṇi?",
"iti64:4.3": "Kāyaduccaritaṃ, vacīduccaritaṃ, manoduccaritaṃ—",
"iti64:4.4": "imāni kho, bhikkhave, tīṇi duccaritānī”ti.",
"iti64:4.5": "Etamatthaṃ bhagavā avoca.",
"iti64:4.6": "Tatthetaṃ iti vuccati:",
"iti64:5.1": "“Kāyaduccaritaṃ katvā,",
"iti64:5.2": "vacīduccaritāni ca;",
"iti64:5.3": "Manoduccaritaṃ katvā,",
"iti64:5.4": "yañcaññaṃ dosasaṃhitaṃ.",
"iti64:6.1": "Akatvā kusalaṃ kammaṃ,",
"iti64:6.2": "katvānākusalaṃ bahuṃ;",
"iti64:6.3": "Kāyassa bhedā duppañño,",
"iti64:6.4": "nirayaṃ sopapajjatī”ti.",
"iti64:7.1": "Ayampi attho vutto bhagavatā, iti me sutanti.",
"iti64:8.1": "Pañcamaṃ."
}