{
"iti95:1.1": "Itivuttaka 95",
"iti95:2.1": "Kāmūpapattisutta",
"iti95:3.1": "Vuttañhetaṃ bhagavatā vuttamarahatāti me sutaṃ:",
"iti95:4.1": "“Tisso imā, bhikkhave, kāmūpapattiyo.",
"iti95:4.2": "Katamā tisso?",
"iti95:4.3": "Paccupaṭṭhitakāmā, nimmānaratino, paranimmitavasavattino—",
"iti95:4.4": "imā kho, bhikkhave, tisso kāmūpapattiyo”ti.",
"iti95:4.5": "Etamatthaṃ bhagavā avoca.",
"iti95:4.6": "Tatthetaṃ iti vuccati:",
"iti95:5.1": "“Paccupaṭṭhitakāmā ca,",
"iti95:5.2": "ye devā vasavattino;",
"iti95:5.3": "Nimmānaratino devā,",
"iti95:5.4": "ye caññe kāmabhogino;",
"iti95:5.5": "Itthabhāvaññathābhāvaṃ,",
"iti95:5.6": "saṃsāraṃ nātivattare.",
"iti95:6.1": "Etamādīnavaṃ ñatvā,",
"iti95:6.2": "kāmabhogesu paṇḍito;",
"iti95:6.3": "Sabbe pariccaje kāme,",
"iti95:6.4": "ye dibbā ye ca mānusā.",
"iti95:7.1": "Piyarūpasātagadhitaṃ,",
"iti95:7.2": "chetvā sotaṃ duraccayaṃ;",
"iti95:7.3": "Asesaṃ parinibbanti,",
"iti95:7.4": "asesaṃ dukkhamaccaguṃ.",
"iti95:8.1": "Ariyaddasā vedaguno,",
"iti95:8.2": "sammadaññāya paṇḍitā;",
"iti95:8.3": "Jātikkhayamabhiññāya,",
"iti95:8.4": "nāgacchanti punabbhavan”ti.",
"iti95:9.1": "Ayampi attho vutto bhagavatā, iti me sutanti.",
"iti95:10.1": "Chaṭṭhaṃ."
}