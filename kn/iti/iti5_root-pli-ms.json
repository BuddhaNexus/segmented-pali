{
"iti5:1.1": "Itivuttaka 5",
"iti5:2.1": "Makkhasutta",
"iti5:3.1": "Vuttañhetaṃ bhagavatā vuttamarahatāti me sutaṃ:",
"iti5:4.1": "“Ekadhammaṃ, bhikkhave, pajahatha;",
"iti5:4.2": "ahaṃ vo pāṭibhogo anāgāmitāya.",
"iti5:4.3": "Katamaṃ ekadhammaṃ?",
"iti5:4.4": "Makkhaṃ, bhikkhave, ekadhammaṃ pajahatha;",
"iti5:4.5": "ahaṃ vo pāṭibhogo anāgāmitāyā”ti.",
"iti5:4.6": "Etamatthaṃ bhagavā avoca.",
"iti5:4.7": "Tatthetaṃ iti vuccati:",
"iti5:5.1": "“Yena makkhena makkhāse,",
"iti5:5.2": "sattā gacchanti duggatiṃ;",
"iti5:5.3": "Taṃ makkhaṃ sammadaññāya,",
"iti5:5.4": "pajahanti vipassino;",
"iti5:5.5": "Pahāya na punāyanti,",
"iti5:5.6": "imaṃ lokaṃ kudācanan”ti.",
"iti5:6.1": "Ayampi attho vutto bhagavatā, iti me sutanti.",
"iti5:7.1": "Pañcamaṃ."
}