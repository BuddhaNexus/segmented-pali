{
"ja336:1.1": "Jātaka",
"ja336:2.1": "Catukkanipāta",
"ja336:3.1": "Kokilavagga",
"ja336:4.1": "6. Brahāchattajātaka",
"ja336:5.1": "“Tiṇaṃ tiṇanti lapasi,",
"ja336:5.2": "ko nu te tiṇamāhari;",
"ja336:5.3": "Kiṃ nu te tiṇakiccatthi,",
"ja336:5.4": "tiṇameva pabhāsasi”.",
"ja336:6.1": "“Idhāgamā brahmacārī,",
"ja336:6.2": "brahā chatto bahussuto;",
"ja336:6.3": "So me sabbaṃ samādāya,",
"ja336:6.4": "tiṇaṃ nikkhippa gacchati”.",
"ja336:7.1": "“Evetaṃ hoti kattabbaṃ,",
"ja336:7.2": "appena bahumicchatā;",
"ja336:7.3": "Sabbaṃ sakassa ādānaṃ,",
"ja336:7.4": "anādānaṃ tiṇassa ca”.",
"ja336:7.5": "()",
"ja336:8.1": "“Sīlavanto na kubbanti,",
"ja336:8.2": "bālo sīlāni kubbati;",
"ja336:8.3": "Aniccasīlaṃ dussīlyaṃ,",
"ja336:8.4": "kiṃ paṇḍiccaṃ karissatī”ti.",
"ja336:9.1": "Brahāchattajātakaṃ chaṭṭhaṃ."
}