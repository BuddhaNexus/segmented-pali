{
"ja232:1.1": "Jātaka",
"ja232:2.1": "Dukanipāta",
"ja232:3.1": "Upāhanavagga",
"ja232:4.1": "2. Vīṇāguṇajātaka",
"ja232:5.1": "“Ekacintito yamattho,",
"ja232:5.2": "bālo apariṇāyako;",
"ja232:5.3": "Na hi khujjena vāmena,",
"ja232:5.4": "bhoti saṅgantumarahasi”.",
"ja232:6.1": "“Purisūsabhaṃ maññamānā,",
"ja232:6.2": "ahaṃ khujjamakāmayiṃ;",
"ja232:6.3": "Soyaṃ saṅkuṭito seti,",
"ja232:6.4": "chinnatanti yathā viṇā”ti.",
"ja232:7.1": "Vīṇāguṇajātakaṃ dutiyaṃ."
}