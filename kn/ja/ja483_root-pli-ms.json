{
"ja483:1.1": "Jātaka",
"ja483:2.1": "Terasakanipāta",
"ja483:3.1": "Ambavagga",
"ja483:4.1": "10. Sarabhamigajātaka",
"ja483:5.1": "“Āsīsetheva puriso,",
"ja483:5.2": "na nibbindeyya paṇḍito;",
"ja483:5.3": "Passāmi vohaṃ attānaṃ,",
"ja483:5.4": "yathā icchiṃ tathā ahu.",
"ja483:6.1": "Āsīsetheva puriso,",
"ja483:6.2": "na nibbindeyya paṇḍito;",
"ja483:6.3": "Passāmi vohaṃ attānaṃ,",
"ja483:6.4": "udakā thalamubbhataṃ.",
"ja483:7.1": "Vāyametheva puriso,",
"ja483:7.2": "na nibbindeyya paṇḍito;",
"ja483:7.3": "Passāmi vohaṃ attānaṃ,",
"ja483:7.4": "yathā icchiṃ tathā ahu.",
"ja483:8.1": "Vāyametheva puriso,",
"ja483:8.2": "na nibbindeyya paṇḍito;",
"ja483:8.3": "Passāmi vohaṃ attānaṃ,",
"ja483:8.4": "udakā thalamubbhataṃ.",
"ja483:9.1": "Dukkhūpanītopi naro sapañño,",
"ja483:9.2": "Āsaṃ na chindeyya sukhāgamāya;",
"ja483:9.3": "Bahū hi phassā ahitā hitā ca,",
"ja483:9.4": "Avitakkitā maccamupabbajanti.",
"ja483:10.1": "Acintitampi bhavati,",
"ja483:10.2": "cintitampi vinassati;",
"ja483:10.3": "Na hi cintāmayā bhogā,",
"ja483:10.4": "itthiyā purisassa vā”.",
"ja483:11.1": "“Sarabhaṃ giriduggasmiṃ,",
"ja483:11.2": "yaṃ tvaṃ anusarī pure;",
"ja483:11.3": "Alīnacittassa tuvaṃ,",
"ja483:11.4": "vikkantamanujīvasi.",
"ja483:12.1": "Yo taṃ viduggā narakā samuddhari,",
"ja483:12.2": "Silāya yoggaṃ sarabho karitvā;",
"ja483:12.3": "Dukkhūpanītaṃ maccumukhā pamocayi,",
"ja483:12.4": "Alīnacittaṃ ta migaṃ vadesi”.",
"ja483:13.1": "“Kiṃ tvaṃ nu tattheva tadā ahosi,",
"ja483:13.2": "Udāhu te koci naṃ etadakkhā;",
"ja483:13.3": "Vivaṭṭacchaddo nusi sabbadassī,",
"ja483:13.4": "Ñāṇaṃ nu te brāhmaṇa bhiṃsarūpaṃ”.",
"ja483:14.1": "“Na cevahaṃ tattha tadā ahosiṃ,",
"ja483:14.2": "Na cāpi me koci naṃ etadakkhā;",
"ja483:14.3": "Gāthāpadānañca subhāsitānaṃ,",
"ja483:14.4": "Atthaṃ tadānenti janinda dhīrā”.",
"ja483:15.1": "“Ādāya pattiṃ paraviriyaghātiṃ,",
"ja483:15.2": "Cāpe saraṃ kiṃ vicikicchase tuvaṃ;",
"ja483:15.3": "Nunno saro sarabhaṃ hantu khippaṃ,",
"ja483:15.4": "Annañhi etaṃ varapañña rañño”.",
"ja483:16.1": "“Addhā pajānāmi ahampi etaṃ,",
"ja483:16.2": "Annaṃ migo brāhmaṇa khattiyassa;",
"ja483:16.3": "Pubbe katañca apacāyamāno,",
"ja483:16.4": "Tasmā migaṃ sarabhaṃ no hanāmi”.",
"ja483:17.1": "“Neso migo mahārāja,",
"ja483:17.2": "asureso disampati;",
"ja483:17.3": "Etaṃ hantvā manussinda,",
"ja483:17.4": "bhavassu amarādhipo.",
"ja483:18.1": "Sace ca rājā vicikicchase tuvaṃ,",
"ja483:18.2": "Hantuṃ migaṃ sarabhaṃ sahāyakaṃ;",
"ja483:18.3": "Saputtadāro naravīraseṭṭha,",
"ja483:18.4": "Gantā tuvaṃ vetaraṇiṃ yamassa”.",
"ja483:19.1": "“Kāmaṃ ahaṃ jānapadā ca sabbe,",
"ja483:19.2": "Puttā ca dārā ca sahāyasaṅghā;",
"ja483:19.3": "Gacchemu taṃ vetaraṇiṃ yamassa,",
"ja483:19.4": "Na tveva hañño mama pāṇado yo.",
"ja483:20.1": "Ayaṃ migo kicchagatassa mayhaṃ,",
"ja483:20.2": "Ekassa kattā vivanasmi ghore;",
"ja483:20.3": "Taṃ tādisaṃ pubbakiccaṃ saranto,",
"ja483:20.4": "Jānaṃ mahābrahme kathaṃ haneyyaṃ”.",
"ja483:21.1": "“Mittābhirādhī cirameva jīva,",
"ja483:21.2": "Rajjaṃ imaṃ dhammaguṇe pasāsa;",
"ja483:21.3": "Nārīgaṇehi paricāriyanto,",
"ja483:21.4": "Modassu raṭṭhe tidiveva vāsavo.",
"ja483:22.1": "Akkodhano niccapasannacitto,",
"ja483:22.2": "Sabbātithī yācayogo bhavitvā;",
"ja483:22.3": "Datvā ca bhutvā ca yathānubhāvaṃ,",
"ja483:22.4": "Anindito saggamupehi ṭhānan”ti. #1930",
"ja483:23.1": "Sarabhamigajātakaṃ dasamaṃ.",
"ja483:24.1": "Terasakanipātaṃ niṭṭhitaṃ.",
"ja483:25.1": "Tassuddānaṃ",
"ja483:26.1": "Varaamba kuṭhāri sahaṃsavaro,",
"ja483:26.2": "Atharaññasmiṃ dūtakapañcamako;",
"ja483:26.3": "Atha bodhi akitti sutakkarinā,",
"ja483:26.4": "Atha rurumigenaparo sarabhoti."
}