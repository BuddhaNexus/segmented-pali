{
"ja314:1.1": "Jātaka",
"ja314:2.1": "Catukkanipāta",
"ja314:3.1": "Pucimandavagga",
"ja314:4.1": "4. Lohakumbhijātaka",
"ja314:5.1": "“Dujjīvitamajīvimha,",
"ja314:5.2": "ye sante na dadamhase;",
"ja314:5.3": "Vijjamānesu bhogesu,",
"ja314:5.4": "dīpaṃ nākamha attano”.",
"ja314:6.1": "“Saṭṭhi vassasahassāni,",
"ja314:6.2": "paripuṇṇāni sabbaso;",
"ja314:6.3": "Niraye paccamānānaṃ,",
"ja314:6.4": "kadā anto bhavissati”.",
"ja314:7.1": "“Natthi anto kuto anto,",
"ja314:7.2": "na anto paṭidissati;",
"ja314:7.3": "Tadā hi pakataṃ pāpaṃ,",
"ja314:7.4": "mama tuyhañca mārisā”.",
"ja314:8.1": "“Sohaṃ nūna ito gantvā,",
"ja314:8.2": "yoniṃ laddhāna mānusiṃ;",
"ja314:8.3": "Vadaññū sīlasampanno,",
"ja314:8.4": "kāhāmi kusalaṃ bahun”ti.",
"ja314:9.1": "Lohakumbhijātakaṃ catutthaṃ."
}