{
"ja79:1.1": "Jātaka",
"ja79:2.1": "Ekakanipāta",
"ja79:3.1": "Varuṇavagga",
"ja79:4.1": "9. Kharassarajātaka",
"ja79:5.1": "“Yato viluttā ca hatā ca gāvo,",
"ja79:5.2": "Daḍḍhāni gehāni jano ca nīto;",
"ja79:5.3": "Athāgamā puttahatāya putto,",
"ja79:5.4": "Kharassaraṃ ḍiṇḍimaṃ vādayanto”ti.",
"ja79:6.1": "Kharassarajātakaṃ navamaṃ."
}