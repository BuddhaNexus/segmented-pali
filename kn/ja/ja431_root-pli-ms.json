{
"ja431:1.1": "Jātaka",
"ja431:2.1": "Navakanipāta",
"ja431:3.1": "Gijjhavagga",
"ja431:4.1": "5. Haritacajātaka",
"ja431:5.1": "“Sutaṃ metaṃ mahābrahme,",
"ja431:5.2": "kāme bhuñjati hārito;",
"ja431:5.3": "Kaccetaṃ vacanaṃ tucchaṃ,",
"ja431:5.4": "kacci suddho iriyyasi”.",
"ja431:6.1": "“Evametaṃ mahārāja,",
"ja431:6.2": "yathā te vacanaṃ sutaṃ;",
"ja431:6.3": "Kummaggaṃ paṭipannosmi,",
"ja431:6.4": "mohaneyyesu mucchito”.",
"ja431:7.1": "“Adu paññā kimatthiyā,",
"ja431:7.2": "nipuṇā sādhucintinī;",
"ja431:7.3": "Yāya uppatitaṃ rāgaṃ,",
"ja431:7.4": "kiṃ mano na vinodaye”.",
"ja431:8.1": "“Cattārome mahārāja,",
"ja431:8.2": "loke atibalā bhusā;",
"ja431:8.3": "Rāgo doso mado moho,",
"ja431:8.4": "yattha paññā na gādhati”.",
"ja431:9.1": "“Arahā sīlasampanno,",
"ja431:9.2": "suddho carati hārito;",
"ja431:9.3": "Medhāvī paṇḍito ceva,",
"ja431:9.4": "iti no sammato bhavaṃ”.",
"ja431:10.1": "“Medhāvīnampi hiṃsanti,",
"ja431:10.2": "isiṃ dhammaguṇe rataṃ;",
"ja431:10.3": "Vitakkā pāpakā rāja,",
"ja431:10.4": "subhā rāgūpasaṃhitā”.",
"ja431:11.1": "“Uppannāyaṃ sarīrajo,",
"ja431:11.2": "Rāgo vaṇṇavidūsano tava;",
"ja431:11.3": "Taṃ pajaha bhaddamatthu te,",
"ja431:11.4": "Bahunnāsi medhāvisammato”.",
"ja431:12.1": "“Te andhakārake kāme,",
"ja431:12.2": "Bahudukkhe mahāvise;",
"ja431:12.3": "Tesaṃ mūlaṃ gavesissaṃ,",
"ja431:12.4": "Checchaṃ rāgaṃ sabandhanaṃ”.",
"ja431:13.1": "“Idaṃ vatvāna hārito,",
"ja431:13.2": "isi saccaparakkamo;",
"ja431:13.3": "Kāmarāgaṃ virājetvā,",
"ja431:13.4": "brahmalokūpago ahū”ti.",
"ja431:14.1": "Haritacajātakaṃ pañcamaṃ."
}