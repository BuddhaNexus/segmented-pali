{
"ja209:1.1": "Jātaka",
"ja209:2.1": "Dukanipāta",
"ja209:3.1": "Nataṃdaḷhavagga",
"ja209:4.1": "9. Kukkuṭajātaka",
"ja209:5.1": "“Diṭṭhā mayā vane rukkhā,",
"ja209:5.2": "assakaṇṇā vibhīṭakā;",
"ja209:5.3": "Na tāni evaṃ sakkanti,",
"ja209:5.4": "yathā tvaṃ rukkha sakkasi”.",
"ja209:6.1": "“Purāṇakukkuṭo ayaṃ,",
"ja209:6.2": "bhetvā pañjaramāgato;",
"ja209:6.3": "Kusalo vāḷapāsānaṃ,",
"ja209:6.4": "apakkamati bhāsatī”ti.",
"ja209:7.1": "Kukkuṭajātakaṃ navamaṃ."
}