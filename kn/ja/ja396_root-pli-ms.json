{
"ja396:1.1": "Jātaka",
"ja396:2.1": "Sattakanipāta",
"ja396:3.1": "Kukkuvagga",
"ja396:4.1": "1. Kukkujātaka",
"ja396:5.1": "“Diyaḍḍhakukkū udayena kaṇṇikā,",
"ja396:5.2": "Vidatthiyo aṭṭha parikkhipanti naṃ;",
"ja396:5.3": "Sā siṃsapā sāramayā apheggukā,",
"ja396:5.4": "Kuhiṃ ṭhitā upparito na dhaṃsati”.",
"ja396:6.1": "“Yā tiṃsati sāramayā anujjukā,",
"ja396:6.2": "Parikiriya gopānasiyo samaṃ ṭhitā;",
"ja396:6.3": "Tāhi susaṅgahitā balasā pīḷitā,",
"ja396:6.4": "Samaṃ ṭhitā upparito na dhaṃsati.",
"ja396:7.1": "Evampi mittehi daḷhehi paṇḍito,",
"ja396:7.2": "Abhejjarūpehi sucīhi mantibhi;",
"ja396:7.3": "Susaṅgahīto siriyā na dhaṃsati,",
"ja396:7.4": "Gopānasī bhāravahāva kaṇṇikā.",
"ja396:8.1": "Kharattacaṃ bellaṃ yathāpi satthavā,",
"ja396:8.2": "Anāmasantopi karoti tittakaṃ;",
"ja396:8.3": "Samāharaṃ sāduṃ karoti patthiva,",
"ja396:8.4": "Asāduṃ kayirā tanubandhamuddharaṃ.",
"ja396:9.1": "Evampi gāmanigamesu paṇḍito,",
"ja396:9.2": "Asāhasaṃ rājadhanāni saṃgharaṃ;",
"ja396:9.3": "Dhammānuvattī paṭipajjamāno,",
"ja396:9.4": "Sa phāti kayirā aviheṭhayaṃ paraṃ.",
"ja396:10.1": "Odātamūlaṃ sucivārisambhavaṃ,",
"ja396:10.2": "Jātaṃ yathā pokkharaṇīsu ambujaṃ;",
"ja396:10.3": "Padumaṃ yathā agginikāsiphālimaṃ,",
"ja396:10.4": "Na kaddamo na rajo na vāri limpati.",
"ja396:11.1": "Evampi vohārasuciṃ asāhasaṃ,",
"ja396:11.2": "Visuddhakammantamapetapāpakaṃ;",
"ja396:11.3": "Na limpati kammakilesa tādiso,",
"ja396:11.4": "Jātaṃ yathā pokkharaṇīsu ambujan”ti.",
"ja396:12.1": "Kukkujātakaṃ paṭhamaṃ."
}