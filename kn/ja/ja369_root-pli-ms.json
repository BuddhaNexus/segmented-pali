{
"ja369:1.1": "Jātaka",
"ja369:2.1": "Pañcakanipāta",
"ja369:3.1": "Vaṇṇārohavagga",
"ja369:4.1": "9. Mittavindakajātaka",
"ja369:5.1": "“Kyāhaṃ devānamakaraṃ,",
"ja369:5.2": "kiṃ pāpaṃ pakataṃ mayā;",
"ja369:5.3": "Yaṃ me sirasmiṃ ohacca,",
"ja369:5.4": "cakkaṃ bhamati matthake”.",
"ja369:6.1": "“Atikkamma ramaṇakaṃ,",
"ja369:6.2": "sadāmattañca dūbhakaṃ;",
"ja369:6.3": "Brahmattarañca pāsādaṃ,",
"ja369:6.4": "kenatthena idhāgato”.",
"ja369:7.1": "“Ito bahutarā bhogā,",
"ja369:7.2": "atra maññe bhavissare;",
"ja369:7.3": "Iti etāya saññāya,",
"ja369:7.4": "passa maṃ byasanaṃ gataṃ”.",
"ja369:8.1": "“Catubbhi aṭṭhajjhagamā,",
"ja369:8.2": "aṭṭhāhipi ca soḷasa;",
"ja369:8.3": "Soḷasāhi ca bāttiṃsa,",
"ja369:8.4": "atricchaṃ cakkamāsado;",
"ja369:8.5": "Icchāhatassa posassa,",
"ja369:8.6": "cakkaṃ bhamati matthake.",
"ja369:9.1": "Uparivisālā duppūrā,",
"ja369:9.2": "icchā visaṭagāminī;",
"ja369:9.3": "Ye ca taṃ anugijjhanti,",
"ja369:9.4": "te honti cakkadhārino”ti.",
"ja369:10.1": "Mittavindakajātakaṃ navamaṃ."
}