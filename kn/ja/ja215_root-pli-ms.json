{
"ja215:1.1": "Jātaka",
"ja215:2.1": "Dukanipāta",
"ja215:3.1": "Bīraṇathambhavagga",
"ja215:4.1": "5. Kacchapajātaka",
"ja215:5.1": "“Avadhī vata attānaṃ,",
"ja215:5.2": "kacchapo byāharaṃ giraṃ;",
"ja215:5.3": "Suggahītasmiṃ kaṭṭhasmiṃ,",
"ja215:5.4": "vācāya sakiyāvadhi.",
"ja215:6.1": "Etampi disvā naravīriyaseṭṭha,",
"ja215:6.2": "Vācaṃ pamuñce kusalaṃ nātivelaṃ;",
"ja215:6.3": "Passasi bahubhāṇena,",
"ja215:6.4": "Kacchapaṃ byasanaṃ gatan”ti.",
"ja215:7.1": "Kacchapajātakaṃ pañcamaṃ."
}