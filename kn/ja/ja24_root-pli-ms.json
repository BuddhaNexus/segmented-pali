{
"ja24:1.1": "Jātaka",
"ja24:2.1": "Ekakanipāta",
"ja24:3.1": "Kuruṅgavagga",
"ja24:4.1": "4. Ājaññajātaka",
"ja24:5.1": "“Yadā yadā yattha yadā,",
"ja24:5.2": "yattha yattha yadā yadā;",
"ja24:5.3": "Ājañño kurute vegaṃ,",
"ja24:5.4": "hāyanti tattha vāḷavā”ti.",
"ja24:6.1": "Ājaññajātakaṃ catutthaṃ."
}