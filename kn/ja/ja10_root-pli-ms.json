{
"ja10:1.1": "Jātaka",
"ja10:2.1": "Ekakanipāta",
"ja10:3.1": "Apaṇṇakavagga",
"ja10:4.1": "10. Sukhavihārijātaka",
"ja10:5.1": "“Yañca aññe na rakkhanti,",
"ja10:5.2": "yo ca aññe na rakkhati;",
"ja10:5.3": "Sa ve rāja sukhaṃ seti,",
"ja10:5.4": "kāmesu anapekkhavā”ti.",
"ja10:6.1": "Sukhavihārijātakaṃ dasamaṃ.",
"ja10:6.2": "Apaṇṇakavaggo paṭhamo.",
"ja10:7.1": "Tassuddānaṃ",
"ja10:8.1": "Varāpaṇṇaka vaṇṇupatha serivaro,",
"ja10:8.2": "Suvicakkhaṇa taṇḍulanāḷikassā;",
"ja10:8.3": "Hiri puttavaruttagāmaṇinā,",
"ja10:8.4": "Yo ca na rakkhati tena dasāti."
}