{
"ja107:1.1": "Jātaka",
"ja107:2.1": "Ekakanipāta",
"ja107:3.1": "Parosatavagga",
"ja107:4.1": "7. Sālittakajātaka",
"ja107:5.1": "“Sādhu kho sippakaṃ nāma,",
"ja107:5.2": "api yādisa kīdisaṃ;",
"ja107:5.3": "Passa khañjappahārena,",
"ja107:5.4": "laddhā gāmā catuddisā”ti.",
"ja107:6.1": "Sālittakajātakaṃ sattamaṃ."
}