{
"ja298:1.1": "Jātaka",
"ja298:2.1": "Tikanipāta",
"ja298:3.1": "Kumbhavagga",
"ja298:4.1": "8. Udumbarajātaka",
"ja298:5.1": "“Udumbarā cime pakkā,",
"ja298:5.2": "nigrodhā ca kapitthanā;",
"ja298:5.3": "Ehi nikkhama bhuñjassu,",
"ja298:5.4": "kiṃ jighacchāya miyyasi”.",
"ja298:6.1": "“Evaṃ so suhito hoti,",
"ja298:6.2": "yo vuḍḍhamapacāyati;",
"ja298:6.3": "Yathāhamajja suhito,",
"ja298:6.4": "dumapakkāni māsito”.",
"ja298:7.1": "“Yaṃ vanejo vanejassa,",
"ja298:7.2": "vañceyya kapino kapi;",
"ja298:7.3": "Daharo kapi saddheyya,",
"ja298:7.4": "na hi jiṇṇo jarākapī”ti.",
"ja298:8.1": "Udumbarajātakaṃ aṭṭhamaṃ."
}