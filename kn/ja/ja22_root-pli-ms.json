{
"ja22:1.1": "Jātaka",
"ja22:2.1": "Ekakanipāta",
"ja22:3.1": "Kuruṅgavagga",
"ja22:4.1": "2. Kukkurajātaka",
"ja22:5.1": "“Ye kukkurā rājakulamhi vaddhā,",
"ja22:5.2": "Koleyyakā vaṇṇabalūpapannā;",
"ja22:5.3": "Teme na vajjhā mayamasma vajjhā,",
"ja22:5.4": "Nāyaṃ saghaccā dubbalaghātikāyan”ti.",
"ja22:6.1": "Kukkurajātakaṃ dutiyaṃ."
}