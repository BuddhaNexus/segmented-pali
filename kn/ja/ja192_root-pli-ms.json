{
"ja192:1.1": "Jātaka",
"ja192:2.1": "Dukanipāta",
"ja192:3.1": "Ruhakavagga",
"ja192:4.1": "2. Sirikāḷakaṇṇijātaka",
"ja192:5.1": "“Itthī siyā rūpavatī,",
"ja192:5.2": "sā ca sīlavatī siyā;",
"ja192:5.3": "Puriso taṃ na iccheyya,",
"ja192:5.4": "saddahāsi mahosadha”.",
"ja192:6.1": "“Saddahāmi mahārāja,",
"ja192:6.2": "puriso dubbhago siyā;",
"ja192:6.3": "Sirī ca kāḷakaṇṇī ca,",
"ja192:6.4": "na samenti kudācanan”ti.",
"ja192:7.1": "Sirikāḷakaṇṇijātakaṃ dutiyaṃ."
}