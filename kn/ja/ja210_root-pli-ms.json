{
"ja210:1.1": "Jātaka",
"ja210:2.1": "Dukanipāta",
"ja210:3.1": "Nataṃdaḷhavagga",
"ja210:4.1": "10. Kandagalakajātaka",
"ja210:5.1": "“Ambho ko nāma yaṃ rukkho,",
"ja210:5.2": "sinnapatto sakaṇṭako;",
"ja210:5.3": "Yattha ekappahārena,",
"ja210:5.4": "uttamaṅgaṃ vibhijjitaṃ”.",
"ja210:6.1": "“Acāri vatāyaṃ vitudaṃ vanāni,",
"ja210:6.2": "Kaṭṭhaṅgarukkhesu asārakesu;",
"ja210:6.3": "Athāsadā khadiraṃ jātasāraṃ,",
"ja210:6.4": "Yatthabbhidā garuḷo uttamaṅgan”ti.",
"ja210:7.1": "Kandagalakajātakaṃ dasamaṃ.",
"ja210:7.2": "Nataṃdaḷhavaggo chaṭṭho.",
"ja210:8.1": "Tassuddānaṃ",
"ja210:9.1": "Daḷhabandhana haṃsavaro ca puna,",
"ja210:9.2": "Virūpakkha saviṭṭhaka macchavaro;",
"ja210:9.3": "Sakuruṅga saassaka ambavaro,",
"ja210:9.4": "Puna kukkuṭako garuḷena dasāti."
}