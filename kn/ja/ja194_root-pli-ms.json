{
"ja194:1.1": "Jātaka",
"ja194:2.1": "Dukanipāta",
"ja194:3.1": "Ruhakavagga",
"ja194:4.1": "4. Maṇicorajātaka",
"ja194:5.1": "“Na santi devā pavasanti nūna,",
"ja194:5.2": "Na hi nūna santi idha lokapālā;",
"ja194:5.3": "Sahasā karontānamasaññatānaṃ,",
"ja194:5.4": "Na hi nūna santī paṭisedhitāro”.",
"ja194:6.1": "“Akāle vassatī tassa,",
"ja194:6.2": "kāle tassa na vassati;",
"ja194:6.3": "Saggā ca cavati ṭhānā,",
"ja194:6.4": "nanu so tāvatā hato”ti.",
"ja194:7.1": "Maṇicorajātakaṃ catutthaṃ."
}