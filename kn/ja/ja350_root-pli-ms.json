{
"ja350:1.1": "Jātaka",
"ja350:2.1": "Catukkanipāta",
"ja350:3.1": "Cūḷakuṇālavagga",
"ja350:4.1": "10. Devatāpañhajātaka",
"ja350:5.1": "“Hanti hatthehi pādehi,",
"ja350:5.2": "mukhañca parisumbhati;",
"ja350:5.3": "Sa ve rāja piyo hoti,",
"ja350:5.4": "kaṃ tena tvābhipassasi”.",
"ja350:6.1": "“Akkosati yathākāmaṃ,",
"ja350:6.2": "āgamañcassa icchati;",
"ja350:6.3": "Sa ve rāja piyo hoti,",
"ja350:6.4": "kaṃ tena tvābhipassasi”.",
"ja350:7.1": "“Abbhakkhāti abhūtena,",
"ja350:7.2": "alikenābhisāraye;",
"ja350:7.3": "Sa ve rāja piyo hoti,",
"ja350:7.4": "kaṃ tena tvābhipassasi”.",
"ja350:8.1": "“Haraṃ annañca pānañca,",
"ja350:8.2": "Vatthasenāsanāni ca;",
"ja350:8.3": "Aññadatthuharā santā,",
"ja350:8.4": "Te ve rāja piyā honti;",
"ja350:8.5": "Kaṃ tena tvābhipassasī”ti. #701",
"ja350:9.1": "Devatāpañhajātakaṃ dasamaṃ.",
"ja350:9.2": "Cūḷakuṇālavaggo pañcamo.",
"ja350:10.1": "Tassuddānaṃ",
"ja350:11.1": "Narānaṃ asakkhivasimhavaro,",
"ja350:11.2": "Nīliyamaggivarañca puna;",
"ja350:11.3": "Puna rasāyasakūṭavaro,",
"ja350:11.4": "Tathārañña sārathi hanti dasāti.",
"ja350:12.1": "Atha vagguddānaṃ",
"ja350:13.1": "Kāliṅgaṃ pucimandañca,",
"ja350:13.2": "kuṭidūsaka kokilā;",
"ja350:13.3": "Cūḷakuṇālavaggo so,",
"ja350:13.4": "pañcamo suppakāsitoti.",
"ja350:14.1": "Catukkanipātaṃ niṭṭhitaṃ."
}