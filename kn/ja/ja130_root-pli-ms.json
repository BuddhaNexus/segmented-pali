{
"ja130:1.1": "Jātaka",
"ja130:2.1": "Ekakanipāta",
"ja130:3.1": "Kusanāḷivagga",
"ja130:4.1": "10. Kosiyajātaka",
"ja130:5.1": "“Yathā vācā ca bhuñjassu,",
"ja130:5.2": "yathā bhuttañca byāhara;",
"ja130:5.3": "Ubhayaṃ te na sameti,",
"ja130:5.4": "vācā bhuttañca kosiye”ti.",
"ja130:6.1": "Kosiyajātakaṃ dasamaṃ.",
"ja130:6.2": "Kusanāḷivaggo terasamo.",
"ja130:7.1": "Tassuddānaṃ",
"ja130:8.1": "Kusanāḷisirivhayano ca yasaṃ,",
"ja130:8.2": "Dadhi mamba kaṭāhakapañcamako;",
"ja130:8.3": "Atha pāpaka khīra biḷāravataṃ,",
"ja130:8.4": "Sikhi kosiyasavhayanena dasāti."
}