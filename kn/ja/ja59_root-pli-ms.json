{
"ja59:1.1": "Jātaka",
"ja59:2.1": "Ekakanipāta",
"ja59:3.1": "Āsīsavagga",
"ja59:4.1": "9. Bherivādakajātaka",
"ja59:5.1": "“Dhame dhame nātidhame,",
"ja59:5.2": "atidhantañhi pāpakaṃ;",
"ja59:5.3": "Dhantena hi sataṃ laddhaṃ,",
"ja59:5.4": "atidhantena nāsitan”ti.",
"ja59:6.1": "Bherivādakajātakaṃ navamaṃ."
}