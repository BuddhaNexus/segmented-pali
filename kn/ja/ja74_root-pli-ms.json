{
"ja74:1.1": "Jātaka",
"ja74:2.1": "Ekakanipāta",
"ja74:3.1": "Varuṇavagga",
"ja74:4.1": "4. Rukkhadhammajātaka",
"ja74:5.1": "“Sādhū sambahulā ñātī,",
"ja74:5.2": "api rukkhā araññajā;",
"ja74:5.3": "Vāto vahati ekaṭṭhaṃ,",
"ja74:5.4": "brahantampi vanappatin”ti.",
"ja74:6.1": "Rukkhadhammajātakaṃ catutthaṃ."
}