{
"ja334:1.1": "Jātaka",
"ja334:2.1": "Catukkanipāta",
"ja334:3.1": "Kokilavagga",
"ja334:4.1": "4. Rājovādajātaka",
"ja334:5.1": "“Gavañce taramānānaṃ,",
"ja334:5.2": "jimhaṃ gacchati puṅgavo;",
"ja334:5.3": "Sabbā tā jimhaṃ gacchanti,",
"ja334:5.4": "nette jimhaṃ gate sati.",
"ja334:6.1": "Evameva manussesu,",
"ja334:6.2": "yo hoti seṭṭhasammato;",
"ja334:6.3": "So ce adhammaṃ carati,",
"ja334:6.4": "pageva itarā pajā;",
"ja334:6.5": "Sabbaṃ raṭṭhaṃ dukhaṃ seti,",
"ja334:6.6": "rājā ce hoti adhammiko.",
"ja334:7.1": "Gavañce taramānānaṃ,",
"ja334:7.2": "ujuṃ gacchati puṅgavo;",
"ja334:7.3": "Sabbā gāvī ujuṃ yanti,",
"ja334:7.4": "nette ujuṃ gate sati.",
"ja334:8.1": "Evameva manussesu,",
"ja334:8.2": "yo hoti seṭṭhasammato;",
"ja334:8.3": "So sace dhammaṃ carati,",
"ja334:8.4": "pageva itarā pajā;",
"ja334:8.5": "Sabbaṃ raṭṭhaṃ sukhaṃ seti,",
"ja334:8.6": "rājā ce hoti dhammiko”ti.",
"ja334:9.1": "Rājovādajātakaṃ catutthaṃ."
}