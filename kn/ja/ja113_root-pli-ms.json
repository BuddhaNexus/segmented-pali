{
"ja113:1.1": "Jātaka",
"ja113:2.1": "Ekakanipāta",
"ja113:3.1": "Haṃcivagga",
"ja113:4.1": "3. Siṅgālajātaka",
"ja113:5.1": "“Saddahāsi siṅgālassa,",
"ja113:5.2": "surāpītassa brāhmaṇa;",
"ja113:5.3": "Sippikānaṃ sataṃ natthi,",
"ja113:5.4": "kuto kaṃsasatā duve”ti.",
"ja113:6.1": "Siṅgālajātakaṃ tatiyaṃ."
}