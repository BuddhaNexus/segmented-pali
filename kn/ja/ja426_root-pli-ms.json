{
"ja426:1.1": "Jātaka",
"ja426:2.1": "Aṭṭhakanipāta",
"ja426:3.1": "Kaccānivagga",
"ja426:4.1": "10. Dīpijātaka",
"ja426:5.1": "“Khamanīyaṃ yāpanīyaṃ,",
"ja426:5.2": "kacci mātula te sukhaṃ;",
"ja426:5.3": "Sukhaṃ te ammā avaca,",
"ja426:5.4": "sukhakāmāva te mayaṃ”.",
"ja426:6.1": "“Naṅguṭṭhaṃ me avakkamma,",
"ja426:6.2": "Heṭhayitvāna eḷike;",
"ja426:6.3": "Sājja mātulavādena,",
"ja426:6.4": "Muñcitabbā nu maññasi”.",
"ja426:7.1": "“Puratthāmukho nisinnosi,",
"ja426:7.2": "Ahaṃ te mukhamāgatā;",
"ja426:7.3": "Pacchato tuyhaṃ naṅguṭṭhaṃ,",
"ja426:7.4": "Kathaṃ khvāhaṃ avakkamiṃ”.",
"ja426:8.1": "“Yāvatā caturo dīpā,",
"ja426:8.2": "sasamuddā sapabbatā;",
"ja426:8.3": "Tāvatā mayhaṃ naṅguṭṭhaṃ,",
"ja426:8.4": "kathaṃ kho tvaṃ vivajjayi”.",
"ja426:9.1": "“Pubbeva metamakkhiṃsu,",
"ja426:9.2": "mātā pitā ca bhātaro;",
"ja426:9.3": "Dīghaṃ duṭṭhassa naṅguṭṭhaṃ,",
"ja426:9.4": "sāmhi vehāyasāgatā”.",
"ja426:10.1": "“Tañca disvāna āyantiṃ,",
"ja426:10.2": "antalikkhasmi eḷike;",
"ja426:10.3": "Migasaṅgho palāyittha,",
"ja426:10.4": "bhakkho me nāsito tayā”.",
"ja426:11.1": "“Iccevaṃ vilapantiyā,",
"ja426:11.2": "eḷakiyā ruhagghaso;",
"ja426:11.3": "Galakaṃ anvāvamaddi,",
"ja426:11.4": "natthi duṭṭhe subhāsitaṃ.",
"ja426:12.1": "Neva duṭṭhe nayo atthi,",
"ja426:12.2": "Na dhammo na subhāsitaṃ;",
"ja426:12.3": "Nikkamaṃ duṭṭhe yuñjetha,",
"ja426:12.4": "So ca sabbhiṃ na rañjatī”ti. #1235",
"ja426:13.1": "Dīpijātakaṃ dasamaṃ.",
"ja426:14.1": "Aṭṭhakanipātaṃ niṭṭhitaṃ.",
"ja426:15.1": "Tassuddānaṃ",
"ja426:16.1": "Parisuddhā manāvilā vatthadharā,",
"ja426:16.2": "Bakarājassa kāyuraṃ daṇḍavaro;",
"ja426:16.3": "Atha aṅgāra cetiya devilinā,",
"ja426:16.4": "Atha āditta gaṅgā daseḷakināti."
}