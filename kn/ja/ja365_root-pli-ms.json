{
"ja365:1.1": "Jātaka",
"ja365:2.1": "Pañcakanipāta",
"ja365:3.1": "Vaṇṇārohavagga",
"ja365:4.1": "5. Ahituṇḍikajātaka",
"ja365:5.1": "“Dhuttomhi samma sumukha,",
"ja365:5.2": "jūte akkhaparājito;",
"ja365:5.3": "Harehi ambapakkāni,",
"ja365:5.4": "vīriyaṃ te bhakkhayāmase”.",
"ja365:6.1": "“Alikaṃ vata maṃ samma,",
"ja365:6.2": "abhūtena pasaṃsasi;",
"ja365:6.3": "Ko te suto vā diṭṭho vā,",
"ja365:6.4": "sumukho nāma makkaṭo.",
"ja365:7.1": "Ajjāpi me taṃ manasi,",
"ja365:7.2": "yaṃ maṃ tvaṃ ahituṇḍika;",
"ja365:7.3": "Dhaññāpaṇaṃ pavisitvā,",
"ja365:7.4": "matto chātaṃ hanāsi maṃ.",
"ja365:8.1": "Tāhaṃ saraṃ dukkhaseyyaṃ,",
"ja365:8.2": "api rajjampi kāraye;",
"ja365:8.3": "Nevāhaṃ yācito dajjaṃ,",
"ja365:8.4": "tathā hi bhayatajjito.",
"ja365:9.1": "Yañca jaññā kule jātaṃ,",
"ja365:9.2": "Gabbhe tittaṃ amacchariṃ;",
"ja365:9.3": "Tena sakhiñca mittañca,",
"ja365:9.4": "Dhīro sandhātumarahatī”ti.",
"ja365:10.1": "Ahituṇḍikajātakaṃ pañcamaṃ."
}