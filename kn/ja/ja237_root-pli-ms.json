{
"ja237:1.1": "Jātaka",
"ja237:2.1": "Dukanipāta",
"ja237:3.1": "Upāhanavagga",
"ja237:4.1": "7. Sāketajātaka",
"ja237:5.1": "“Ko nu kho bhagavā hetu,",
"ja237:5.2": "ekacce idha puggale;",
"ja237:5.3": "Atīva hadayaṃ nibbāti,",
"ja237:5.4": "cittañcāpi pasīdati”.",
"ja237:6.1": "“Pubbeva sannivāsena,",
"ja237:6.2": "paccuppannahitena vā;",
"ja237:6.3": "Evaṃ taṃ jāyate pemaṃ,",
"ja237:6.4": "uppalaṃva yathodake”ti.",
"ja237:7.1": "Sāketajātakaṃ sattamaṃ."
}