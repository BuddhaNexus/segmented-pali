{
"ja214:1.1": "Jātaka",
"ja214:2.1": "Dukanipāta",
"ja214:3.1": "Bīraṇathambhavagga",
"ja214:4.1": "4. Puṇṇanadījātaka",
"ja214:5.1": "“Puṇṇaṃ nadiṃ yena ca peyyamāhu,",
"ja214:5.2": "Jātaṃ yavaṃ yena ca guyhamāhu;",
"ja214:5.3": "Dūraṃ gataṃ yena ca avhayanti,",
"ja214:5.4": "So tyāgato handa ca bhuñja brāhmaṇa”.",
"ja214:6.1": "“Yato maṃ saratī rājā,",
"ja214:6.2": "vāyasampi pahetave;",
"ja214:6.3": "Haṃsā koñcā mayūrā ca,",
"ja214:6.4": "asatīyeva pāpiyā”ti.",
"ja214:7.1": "Puṇṇanadījātakaṃ catutthaṃ."
}