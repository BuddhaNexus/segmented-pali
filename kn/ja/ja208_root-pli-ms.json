{
"ja208:1.1": "Jātaka",
"ja208:2.1": "Dukanipāta",
"ja208:3.1": "Nataṃdaḷhavagga",
"ja208:4.1": "8. Susumārajātaka",
"ja208:5.1": "“Alaṃ metehi ambehi,",
"ja208:5.2": "jambūhi panasehi ca;",
"ja208:5.3": "Yāni pāraṃ samuddassa,",
"ja208:5.4": "varaṃ mayhaṃ udumbaro.",
"ja208:6.1": "Mahatī vata te bondi,",
"ja208:6.2": "na ca paññā tadūpikā;",
"ja208:6.3": "Susumāra vañcito mesi,",
"ja208:6.4": "gaccha dāni yathāsukhan”ti.",
"ja208:7.1": "Susumārajātakaṃ aṭṭhamaṃ."
}