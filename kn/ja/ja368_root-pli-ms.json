{
"ja368:1.1": "Jātaka",
"ja368:2.1": "Pañcakanipāta",
"ja368:3.1": "Vaṇṇārohavagga",
"ja368:4.1": "8. Tacasārajātaka",
"ja368:5.1": "“Amittahatthatthagatā,",
"ja368:5.2": "tacasārasamappitā;",
"ja368:5.3": "Pasannamukhavaṇṇāttha,",
"ja368:5.4": "kasmā tumhe na socatha”.",
"ja368:6.1": "“Na socanāya paridevanāya,",
"ja368:6.2": "Atthova labbho api appakopi;",
"ja368:6.3": "Socantamenaṃ dukhitaṃ viditvā,",
"ja368:6.4": "Paccatthikā attamanā bhavanti.",
"ja368:7.1": "Yato ca kho paṇḍito āpadāsu,",
"ja368:7.2": "Na vedhatī atthavinicchayaññū;",
"ja368:7.3": "Paccatthikāssa dukhitā bhavanti,",
"ja368:7.4": "Disvā mukhaṃ avikāraṃ purāṇaṃ.",
"ja368:8.1": "Jappena mantena subhāsitena,",
"ja368:8.2": "Anuppadānena paveṇiyā vā;",
"ja368:8.3": "Yathā yathā yattha labhetha atthaṃ,",
"ja368:8.4": "Tathā tathā tattha parakkameyya.",
"ja368:9.1": "Yato ca jāneyya alabbhaneyyo,",
"ja368:9.2": "Mayā va aññena vā esa attho;",
"ja368:9.3": "Asocamāno adhivāsayeyya,",
"ja368:9.4": "Kammaṃ daḷhaṃ kinti karomi dānī”ti.",
"ja368:10.1": "Tacasārajātakaṃ aṭṭhamaṃ."
}