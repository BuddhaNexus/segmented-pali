{
"ja56:1.1": "Jātaka",
"ja56:2.1": "Ekakanipāta",
"ja56:3.1": "Āsīsavagga",
"ja56:4.1": "6. Kañcanakkhandhajātaka",
"ja56:5.1": "“Yo pahaṭṭhena cittena,",
"ja56:5.2": "pahaṭṭhamanaso naro;",
"ja56:5.3": "Bhāveti kusalaṃ dhammaṃ,",
"ja56:5.4": "yogakkhemassa pattiyā;",
"ja56:5.5": "Pāpuṇe anupubbena,",
"ja56:5.6": "sabbasaṃyojanakkhayan”ti.",
"ja56:6.1": "Kañcanakkhandhajātakaṃ chaṭṭhaṃ."
}