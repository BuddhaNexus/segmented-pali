{
"ja163:1.1": "Jātaka",
"ja163:2.1": "Dukanipāta",
"ja163:3.1": "Santhavavagga",
"ja163:4.1": "3. Susīmajātaka",
"ja163:5.1": "“Kāḷā migā setadantā tavīme,",
"ja163:5.2": "Parosataṃ hemajālābhichannā;",
"ja163:5.3": "Te te dadāmīti susīma brūsi,",
"ja163:5.4": "Anussaraṃ pettipitāmahānaṃ”.",
"ja163:6.1": "“Kāḷā migā setadantā mamīme,",
"ja163:6.2": "Parosataṃ hemajālābhichannā;",
"ja163:6.3": "Te te dadāmīti vadāmi māṇava,",
"ja163:6.4": "Anussaraṃ pettipitāmahānan”ti.",
"ja163:7.1": "Susīmajātakaṃ tatiyaṃ."
}