{
"ja268:1.1": "Jātaka",
"ja268:2.1": "Tikanipāta",
"ja268:3.1": "Padumavagga",
"ja268:4.1": "8. Ārāmadūsakajātaka",
"ja268:5.1": "“Yo ve sabbasametānaṃ,",
"ja268:5.2": "ahuvā seṭṭhasammato;",
"ja268:5.3": "Tassāyaṃ edisī paññā,",
"ja268:5.4": "kimeva itarā pajā”.",
"ja268:6.1": "“Evameva tuvaṃ brahme,",
"ja268:6.2": "anaññāya vinindasi;",
"ja268:6.3": "Kathaṃ mūlaṃ adisvāna,",
"ja268:6.4": "rukkhaṃ jaññā patiṭṭhitaṃ”.",
"ja268:7.1": "“Nāhaṃ tumhe vinindāmi,",
"ja268:7.2": "ye caññe vānarā vane;",
"ja268:7.3": "Vissasenova gārayho,",
"ja268:7.4": "yassatthā rukkharopakā”ti.",
"ja268:8.1": "Ārāmadūsakajātakaṃ aṭṭhamaṃ."
}