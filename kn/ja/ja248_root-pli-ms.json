{
"ja248:1.1": "Jātaka",
"ja248:2.1": "Dukanipāta",
"ja248:3.1": "Siṅgālavagga",
"ja248:4.1": "8. Kiṃsukopamajātaka",
"ja248:5.1": "“Sabbehi kiṃsuko diṭṭho,",
"ja248:5.2": "kinnvettha vicikicchatha;",
"ja248:5.3": "Na hi sabbesu ṭhānesu,",
"ja248:5.4": "sārathī paripucchito”.",
"ja248:6.1": "“Evaṃ sabbehi ñāṇehi,",
"ja248:6.2": "yesaṃ dhammā ajānitā;",
"ja248:6.3": "Te ve dhammesu kaṅkhanti,",
"ja248:6.4": "kiṃsukasmiṃva bhātaro”ti.",
"ja248:7.1": "Kiṃsukopamajātakaṃ aṭṭhamaṃ."
}