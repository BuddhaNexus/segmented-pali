{
"ja323:1.1": "Jātaka",
"ja323:2.1": "Catukkanipāta",
"ja323:3.1": "Kuṭidūsakavagga",
"ja323:4.1": "3. Brahmadattajātaka",
"ja323:5.1": "“Dvayaṃ yācanako rāja,",
"ja323:5.2": "brahmadatta nigacchati;",
"ja323:5.3": "Alābhaṃ dhanalābhaṃ vā,",
"ja323:5.4": "evaṃ dhammā hi yācanā.",
"ja323:6.1": "Yācanaṃ rodanaṃ āhu,",
"ja323:6.2": "pañcālānaṃ rathesabha;",
"ja323:6.3": "Yo yācanaṃ paccakkhāti,",
"ja323:6.4": "tamāhu paṭirodanaṃ.",
"ja323:7.1": "Mā maddasaṃsu rodantaṃ,",
"ja323:7.2": "pañcālā susamāgatā;",
"ja323:7.3": "Tuvaṃ vā paṭirodantaṃ,",
"ja323:7.4": "tasmā icchāmahaṃ raho”.",
"ja323:8.1": "“Dadāmi te brāhmaṇa rohiṇīnaṃ,",
"ja323:8.2": "Gavaṃ sahassaṃ saha puṅgavena;",
"ja323:8.3": "Ariyo hi ariyassa kathaṃ na dajjā,",
"ja323:8.4": "Sutvāna gāthā tava dhammayuttā”ti.",
"ja323:9.1": "Brahmadattajātakaṃ tatiyaṃ."
}