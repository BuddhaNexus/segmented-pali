{
"ja265:1.1": "Jātaka",
"ja265:2.1": "Tikanipāta",
"ja265:3.1": "Padumavagga",
"ja265:4.1": "5. Khurappajātaka",
"ja265:5.1": "“Disvā khurappe dhanuveganunne,",
"ja265:5.2": "Khagge gahīte tikhiṇe teladhote;",
"ja265:5.3": "Tasmiṃ bhayasmiṃ maraṇe viyūḷhe,",
"ja265:5.4": "Kasmā nu te nāhu chambhitattaṃ”.",
"ja265:6.1": "“Disvā khurappe dhanuveganunne,",
"ja265:6.2": "Khagge gahīte tikhiṇe teladhote;",
"ja265:6.3": "Tasmiṃ bhayasmiṃ maraṇe viyūḷhe,",
"ja265:6.4": "Vedaṃ alatthaṃ vipulaṃ uḷāraṃ.",
"ja265:7.1": "So vedajāto ajjhabhaviṃ amitte,",
"ja265:7.2": "Pubbeva me jīvitamāsi cattaṃ;",
"ja265:7.3": "Na hi jīvite ālayaṃ kubbamāno,",
"ja265:7.4": "Sūro kayirā sūrakiccaṃ kadācī”ti.",
"ja265:8.1": "Khurappajātakaṃ pañcamaṃ."
}