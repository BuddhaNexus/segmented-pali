{
"ja251:1.1": "Jātaka",
"ja251:2.1": "Tikanipāta",
"ja251:3.1": "Saṅkappavagga",
"ja251:4.1": "1. Saṅkapparāgajātaka",
"ja251:5.1": "“Saṅkapparāgadhotena,",
"ja251:5.2": "vitakkanisitena ca;",
"ja251:5.3": "Nālaṅkatena bhadrena,",
"ja251:5.4": "usukārākatena ca.",
"ja251:6.1": "Na kaṇṇāyatamuttena,",
"ja251:6.2": "nāpi morūpasevinā;",
"ja251:6.3": "Tenamhi hadaye viddho,",
"ja251:6.4": "sabbaṅgaparidāhinā.",
"ja251:7.1": "Āvedhañca na passāmi,",
"ja251:7.2": "Yato ruhiramassave;",
"ja251:7.3": "Yāva ayoniso cittaṃ,",
"ja251:7.4": "Sayaṃ me dukkhamābhatan”ti.",
"ja251:8.1": "Saṅkapparāgajātakaṃ paṭhamaṃ."
}