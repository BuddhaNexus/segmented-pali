{
"ja96:1.1": "Jātaka",
"ja96:2.1": "Ekakanipāta",
"ja96:3.1": "Littavagga",
"ja96:4.1": "6. Telapattajātaka",
"ja96:5.1": "“Samatittikaṃ anavasekaṃ,",
"ja96:5.2": "Telapattaṃ yathā parihareyya;",
"ja96:5.3": "Evaṃ sacittamanurakkhe,",
"ja96:5.4": "Patthayāno disaṃ agatapubban”ti.",
"ja96:6.1": "Telapattajātakaṃ chaṭṭhaṃ."
}