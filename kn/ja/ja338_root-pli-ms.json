{
"ja338:1.1": "Jātaka",
"ja338:2.1": "Catukkanipāta",
"ja338:3.1": "Kokilavagga",
"ja338:4.1": "8. Thusajātaka",
"ja338:5.1": "“Viditaṃ thusaṃ undurānaṃ,",
"ja338:5.2": "Viditaṃ pana taṇḍulaṃ;",
"ja338:5.3": "Thusaṃ thusaṃ vivajjetvā,",
"ja338:5.4": "Taṇḍulaṃ pana khādare.",
"ja338:6.1": "Yā mantanā araññasmiṃ,",
"ja338:6.2": "yā ca gāme nikaṇṇikā;",
"ja338:6.3": "Yañcetaṃ iti cīti ca,",
"ja338:6.4": "etampi viditaṃ mayā.",
"ja338:7.1": "Dhammena kira jātassa,",
"ja338:7.2": "pitā puttassa makkaṭo;",
"ja338:7.3": "Daharasseva santassa,",
"ja338:7.4": "dantehi phalamacchidā.",
"ja338:8.1": "Yametaṃ parisappasi,",
"ja338:8.2": "ajakāṇova sāsape;",
"ja338:8.3": "Yopāyaṃ heṭṭhato seti,",
"ja338:8.4": "etampi viditaṃ mayā”ti.",
"ja338:9.1": "Thusajātakaṃ aṭṭhamaṃ."
}