{
"ja167:1.1": "Jātaka",
"ja167:2.1": "Dukanipāta",
"ja167:3.1": "Santhavavagga",
"ja167:4.1": "7. Samiddhijātaka",
"ja167:5.1": "“Abhutvā bhikkhasi bhikkhu,",
"ja167:5.2": "Na hi bhutvāna bhikkhasi;",
"ja167:5.3": "Bhutvāna bhikkhu bhikkhassu,",
"ja167:5.4": "Mā taṃ kālo upaccagā.",
"ja167:6.1": "Kālaṃ vohaṃ na jānāmi,",
"ja167:6.2": "channo kālo na dissati;",
"ja167:6.3": "Tasmā abhutvā bhikkhāmi,",
"ja167:6.4": "mā maṃ kālo upaccagā”ti.",
"ja167:7.1": "Samiddhijātakaṃ sattamaṃ."
}