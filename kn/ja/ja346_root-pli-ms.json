{
"ja346:1.1": "Jātaka",
"ja346:2.1": "Catukkanipāta",
"ja346:3.1": "Cūḷakuṇālavagga",
"ja346:4.1": "6. Kesavajātaka",
"ja346:5.1": "“Manussindaṃ jahitvāna,",
"ja346:5.2": "sabbakāmasamiddhinaṃ;",
"ja346:5.3": "Kathaṃ nu bhagavā kesī,",
"ja346:5.4": "kappassa ramati assame”.",
"ja346:6.1": "“Sādūni ramaṇīyāni,",
"ja346:6.2": "santi vakkhā manoramā;",
"ja346:6.3": "Subhāsitāni kappassa,",
"ja346:6.4": "nārada ramayanti maṃ”.",
"ja346:7.1": "“Sālīnaṃ odanaṃ bhuñje,",
"ja346:7.2": "suciṃ maṃsūpasecanaṃ;",
"ja346:7.3": "Kathaṃ sāmākanīvāraṃ,",
"ja346:7.4": "aloṇaṃ chādayanti taṃ”.",
"ja346:8.1": "“Sāduṃ vā yadi vāsāduṃ,",
"ja346:8.2": "appaṃ vā yadi vā bahuṃ;",
"ja346:8.3": "Vissattho yattha bhuñjeyya,",
"ja346:8.4": "vissāsaparamā rasā”ti.",
"ja346:9.1": "Kesavajātakaṃ chaṭṭhaṃ."
}