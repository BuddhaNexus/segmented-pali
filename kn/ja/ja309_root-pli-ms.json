{
"ja309:1.1": "Jātaka",
"ja309:2.1": "Catukkanipāta",
"ja309:3.1": "Kāliṅgavagga",
"ja309:4.1": "9. Chavakajātaka",
"ja309:5.1": "“Sabbamidaṃ carimaṃ kataṃ,",
"ja309:5.2": "Ubho dhammaṃ na passare;",
"ja309:5.3": "Ubho pakatiyā cutā,",
"ja309:5.4": "Yo cāyaṃ mantejjhāpeti;",
"ja309:5.5": "Yo ca mantaṃ adhīyati”.",
"ja309:6.1": "“Sālīnaṃ odanaṃ bhuñje,",
"ja309:6.2": "suciṃ maṃsūpasecanaṃ;",
"ja309:6.3": "Tasmā etaṃ na sevāmi,",
"ja309:6.4": "dhammaṃ isīhi sevitaṃ”.",
"ja309:7.1": "“Paribbaja mahā loko,",
"ja309:7.2": "pacantaññepi pāṇino;",
"ja309:7.3": "Mā taṃ adhammo ācarito,",
"ja309:7.4": "asmā kumbhamivābhidā.",
"ja309:8.1": "Dhiratthu taṃ yasalābhaṃ,",
"ja309:8.2": "dhanalābhañca brāhmaṇa;",
"ja309:8.3": "Yā vutti vinipātena,",
"ja309:8.4": "adhammacaraṇena vā”ti.",
"ja309:9.1": "Chavakajātakaṃ navamaṃ."
}