{
"ja217:1.1": "Jātaka",
"ja217:2.1": "Dukanipāta",
"ja217:3.1": "Bīraṇathambhavagga",
"ja217:4.1": "7. Seggujātaka",
"ja217:5.1": "“Sabbo loko attamano ahosi,",
"ja217:5.2": "Akovidā gāmadhammassa seggu;",
"ja217:5.3": "Komāri ko nāma tavajja dhammo,",
"ja217:5.4": "Yaṃ tvaṃ gahitā pavane parodasi”.",
"ja217:6.1": "“Yo dukkhaphuṭṭhāya bhaveyya tāṇaṃ,",
"ja217:6.2": "So me pitā dubbhi vane karoti;",
"ja217:6.3": "Sā kassa kandāmi vanassa majjhe,",
"ja217:6.4": "Yo tāyitā so sahasaṃ karotī”ti.",
"ja217:7.1": "Seggujātakaṃ sattamaṃ."
}