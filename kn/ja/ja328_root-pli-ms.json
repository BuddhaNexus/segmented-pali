{
"ja328:1.1": "Jātaka",
"ja328:2.1": "Catukkanipāta",
"ja328:3.1": "Kuṭidūsakavagga",
"ja328:4.1": "8. Ananusociyajātaka",
"ja328:5.1": "“Bahūnaṃ vijjatī bhotī,",
"ja328:5.2": "tehi me kiṃ bhavissati;",
"ja328:5.3": "Tasmā etaṃ na socāmi,",
"ja328:5.4": "piyaṃ sammillahāsiniṃ.",
"ja328:6.1": "Taṃ tañce anusoceyya,",
"ja328:6.2": "yaṃ yaṃ tassa na vijjati;",
"ja328:6.3": "Attānamanusoceyya,",
"ja328:6.4": "sadā maccuvasaṃ pataṃ.",
"ja328:7.1": "Na heva ṭhitaṃ nāsīnaṃ,",
"ja328:7.2": "Na sayānaṃ na paddhaguṃ;",
"ja328:7.3": "Yāva byāti nimisati,",
"ja328:7.4": "Tatrāpi rasatī vayo.",
"ja328:8.1": "Tatthattani vatappaddhe,",
"ja328:8.2": "vinābhāve asaṃsaye;",
"ja328:8.3": "Bhūtaṃ sesaṃ dayitabbaṃ,",
"ja328:8.4": "vītaṃ ananusociyan”ti.",
"ja328:9.1": "Ananusociyajātakaṃ aṭṭhamaṃ."
}