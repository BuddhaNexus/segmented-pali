{
"ja187:1.1": "Jātaka",
"ja187:2.1": "Dukanipāta",
"ja187:3.1": "Asadisavagga",
"ja187:4.1": "7. Catumaṭṭhajātaka",
"ja187:5.1": "“Ucce viṭabhimāruyha,",
"ja187:5.2": "mantayavho rahogatā;",
"ja187:5.3": "Nīce oruyha mantavho,",
"ja187:5.4": "migarājāpi sossati.",
"ja187:6.1": "Yaṃ suvaṇṇo suvaṇṇena,",
"ja187:6.2": "devo devena mantaye;",
"ja187:6.3": "Kiṃ tettha catumaṭṭhassa,",
"ja187:6.4": "bilaṃ pavisa jambukā”ti.",
"ja187:7.1": "Catumaṭṭhajātakaṃ sattamaṃ."
}