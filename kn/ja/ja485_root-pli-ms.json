{
"ja485:1.1": "Jātaka",
"ja485:2.1": "Pakiṇṇakanipāta",
"ja485:3.1": "Sālikedāravagga",
"ja485:4.1": "2. Candakinnarījātaka",
"ja485:5.1": "“Upanīyatidaṃ maññe,",
"ja485:5.2": "cande lohitamaddane;",
"ja485:5.3": "Ajja jahāmi jīvitaṃ,",
"ja485:5.4": "pāṇā me cande nirujjhanti.",
"ja485:6.1": "Osīdi me dukkhaṃ hadayaṃ,",
"ja485:6.2": "Me ḍayhate nitammāmi;",
"ja485:6.3": "Tava candiyā socantiyā,",
"ja485:6.4": "Na naṃ aññehi sokehi.",
"ja485:7.1": "Tiṇamiva vanamiva milāyāmi,",
"ja485:7.2": "Nadī aparipuṇṇāva sussāmi;",
"ja485:7.3": "Tava candiyā socantiyā,",
"ja485:7.4": "Na naṃ aññehi sokehi.",
"ja485:8.1": "Vassamiva sare pāde,",
"ja485:8.2": "Imāni assūni vattare mayhaṃ;",
"ja485:8.3": "Tava candiyā socantiyā,",
"ja485:8.4": "Na naṃ aññehi sokehi”.",
"ja485:9.1": "“Pāpo khosi rājaputta,",
"ja485:9.2": "Yo me icchitaṃ patiṃ varākiyā;",
"ja485:9.3": "Vijjhasi vanamūlasmiṃ,",
"ja485:9.4": "Soyaṃ viddho chamā seti.",
"ja485:10.1": "Imaṃ mayhaṃ hadayasokaṃ,",
"ja485:10.2": "Paṭimuñcatu rājaputta tava mātā;",
"ja485:10.3": "Yo mayhaṃ hadayasoko,",
"ja485:10.4": "Kimpurisaṃ avekkhamānāya.",
"ja485:11.1": "Imaṃ mayhaṃ hadayasokaṃ,",
"ja485:11.2": "Paṭimuñcatu rājaputta tava jāyā;",
"ja485:11.3": "Yo mayhaṃ hadayasoko,",
"ja485:11.4": "Kimpurisaṃ avekkhamānāya.",
"ja485:12.1": "Mā ca puttaṃ mā ca patiṃ,",
"ja485:12.2": "Addakkhi rājaputta tava mātā;",
"ja485:12.3": "Yo kimpurisaṃ avadhi,",
"ja485:12.4": "Adūsakaṃ mayha kāmā hi.",
"ja485:13.1": "Mā ca puttaṃ mā ca patiṃ,",
"ja485:13.2": "Addakkhi rājaputta tava jāyā;",
"ja485:13.3": "Yo kimpurisaṃ avadhi,",
"ja485:13.4": "Adūsakaṃ mayha kāmā hi”.",
"ja485:14.1": "“Mā tvaṃ cande rodi,",
"ja485:14.2": "Mā soci vanatimiramattakkhi;",
"ja485:14.3": "Mama tvaṃ hehisi bhariyā,",
"ja485:14.4": "Rājakule pūjitā nārībhi”.",
"ja485:15.1": "“Api nūnahaṃ marissaṃ,",
"ja485:15.2": "Nāhaṃ rājaputta tava hessaṃ;",
"ja485:15.3": "Yo kimpurisaṃ avadhi,",
"ja485:15.4": "Adūsakaṃ mayha kāmā hi”.",
"ja485:16.1": "“Api bhīruke api jīvitukāmike,",
"ja485:16.2": "Kimpurisi gaccha himavantaṃ;",
"ja485:16.3": "Tālīsatagarabhojanā,",
"ja485:16.4": "Aññe taṃ migā ramissanti”.",
"ja485:17.1": "“Te pabbatā tā ca kandarā,",
"ja485:17.2": "Tā ca giriguhāyo tatheva tiṭṭhanti;",
"ja485:17.3": "Tattheva taṃ apassantī,",
"ja485:17.4": "Kimpurisa kathaṃ ahaṃ kassaṃ.",
"ja485:18.1": "Te paṇṇasanthatā ramaṇīyā,",
"ja485:18.2": "Vāḷamigehi anuciṇṇā;",
"ja485:18.3": "Tattheva taṃ apassantī,",
"ja485:18.4": "Kimpurisa kathaṃ ahaṃ kassaṃ.",
"ja485:19.1": "Te pupphasanthatā ramaṇīyā,",
"ja485:19.2": "Vāḷamigehi anuciṇṇā;",
"ja485:19.3": "Tattheva taṃ apassantī,",
"ja485:19.4": "Kimpurisa kathaṃ ahaṃ kassaṃ.",
"ja485:20.1": "Acchā savanti girivananadiyo,",
"ja485:20.2": "Kusumābhikiṇṇasotāyo;",
"ja485:20.3": "Tattheva taṃ apassantī,",
"ja485:20.4": "Kimpurisa kathaṃ ahaṃ kassaṃ.",
"ja485:21.1": "Nīlāni himavato pabbatassa,",
"ja485:21.2": "Kūṭāni dassanīyāni;",
"ja485:21.3": "Tattheva taṃ apassantī,",
"ja485:21.4": "Kimpurisa kathaṃ ahaṃ kassaṃ.",
"ja485:22.1": "Pītāni himavato pabbatassa,",
"ja485:22.2": "Kūṭāni dassanīyāni;",
"ja485:22.3": "Tattheva taṃ apassantī,",
"ja485:22.4": "Kimpurisa kathaṃ ahaṃ kassaṃ.",
"ja485:23.1": "Tambāni himavato pabbatassa,",
"ja485:23.2": "Kūṭāni dassanīyāni;",
"ja485:23.3": "Tattheva taṃ apassantī,",
"ja485:23.4": "Kimpurisa kathaṃ ahaṃ kassaṃ.",
"ja485:24.1": "Tuṅgāni himavato pabbatassa,",
"ja485:24.2": "Kūṭāni dassanīyāni;",
"ja485:24.3": "Tattheva taṃ apassantī,",
"ja485:24.4": "Kimpurisa kathaṃ ahaṃ kassaṃ.",
"ja485:25.1": "Setāni himavato pabbatassa,",
"ja485:25.2": "Kūṭāni dassanīyāni;",
"ja485:25.3": "Tattheva taṃ apassantī,",
"ja485:25.4": "Kimpurisa kathaṃ ahaṃ kassaṃ.",
"ja485:26.1": "Citrāni himavato pabbatassa,",
"ja485:26.2": "Kūṭāni dassanīyāni;",
"ja485:26.3": "Tattheva taṃ apassantī,",
"ja485:26.4": "Kimpurisa kathaṃ ahaṃ kassaṃ.",
"ja485:27.1": "Yakkhagaṇasevite gandhamādane,",
"ja485:27.2": "Osadhebhi sañchanne;",
"ja485:27.3": "Tattheva taṃ apassantī,",
"ja485:27.4": "Kimpurisa kathaṃ ahaṃ kassaṃ.",
"ja485:28.1": "Kimpurisasevite gandhamādane,",
"ja485:28.2": "Osadhebhi sañchanne;",
"ja485:28.3": "Tattheva taṃ apassantī,",
"ja485:28.4": "Kimpurisa kathaṃ ahaṃ kassaṃ”.",
"ja485:29.1": "“Vande te ayirabrahme,",
"ja485:29.2": "Yo me icchitaṃ patiṃ varākiyā;",
"ja485:29.3": "Amatena abhisiñci,",
"ja485:29.4": "Samāgatāsmi piyatamena.",
"ja485:30.1": "Vicarāma dāni girivana nadiyo,",
"ja485:30.2": "Kusumābhikiṇṇasotāyo;",
"ja485:30.3": "Nānādumavasanāyo,",
"ja485:30.4": "Piyaṃvadā aññamaññassā”ti.",
"ja485:31.1": "Candakinnarījātakaṃ dutiyaṃ."
}