{
"ja349:1.1": "Jātaka",
"ja349:2.1": "Catukkanipāta",
"ja349:3.1": "Cūḷakuṇālavagga",
"ja349:4.1": "9. Sandhibhedajātaka",
"ja349:5.1": "“Neva itthīsu sāmaññaṃ,",
"ja349:5.2": "nāpi bhakkhesu sārathi;",
"ja349:5.3": "Athassa sandhibhedassa,",
"ja349:5.4": "passa yāva sucintitaṃ.",
"ja349:6.1": "Asi tikkhova maṃsamhi,",
"ja349:6.2": "pesuññaṃ parivattati;",
"ja349:6.3": "Yatthūsabhañca sīhañca,",
"ja349:6.4": "bhakkhayanti migādhamā.",
"ja349:7.1": "Imaṃ so sayanaṃ seti,",
"ja349:7.2": "yamimaṃ passasi sārathi;",
"ja349:7.3": "Yo vācaṃ sandhibhedassa,",
"ja349:7.4": "pisuṇassa nibodhati.",
"ja349:8.1": "Te janā sukhamedhanti,",
"ja349:8.2": "narā saggagatāriva;",
"ja349:8.3": "Ye vācaṃ sandhibhedassa,",
"ja349:8.4": "nāvabodhanti sārathī”ti.",
"ja349:9.1": "Sandhibhedajātakaṃ navamaṃ."
}