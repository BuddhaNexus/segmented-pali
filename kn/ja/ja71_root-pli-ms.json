{
"ja71:1.1": "Jātaka",
"ja71:2.1": "Ekakanipāta",
"ja71:3.1": "Varuṇavagga",
"ja71:4.1": "1. Varuṇajātaka",
"ja71:5.1": "“Yo pubbe karaṇīyāni,",
"ja71:5.2": "pacchā so kātumicchati;",
"ja71:5.3": "Varuṇakaṭṭhabhañjova,",
"ja71:5.4": "sa pacchā manutappatī”ti.",
"ja71:6.1": "Varuṇajātakaṃ paṭhamaṃ."
}