{
"ja398:1.1": "Jātaka",
"ja398:2.1": "Sattakanipāta",
"ja398:3.1": "Kukkuvagga",
"ja398:4.1": "3. Sutanujātaka",
"ja398:5.1": "“Rājā te bhattaṃ pāhesi,",
"ja398:5.2": "Suciṃ maṃsūpasecanaṃ;",
"ja398:5.3": "Maghadevasmiṃ adhivatthe,",
"ja398:5.4": "Ehi nikkhamma bhuñjasu”.",
"ja398:6.1": "“Ehi māṇava orena,",
"ja398:6.2": "bhikkhamādāya sūpitaṃ;",
"ja398:6.3": "Tvañca māṇava bhikkhā ca,",
"ja398:6.4": "ubho bhakkhā bhavissatha”.",
"ja398:7.1": "“Appakena tuvaṃ yakkha,",
"ja398:7.2": "thullamatthaṃ jahissasi;",
"ja398:7.3": "Bhikkhaṃ te nāharissanti,",
"ja398:7.4": "janā maraṇasaññino.",
"ja398:8.1": "Laddhāya yakkhā tava niccabhikkhaṃ,",
"ja398:8.2": "Suciṃ paṇītaṃ rasasā upetaṃ;",
"ja398:8.3": "Bhikkhañca te āhariyo naro idha,",
"ja398:8.4": "Sudullabho hehiti bhakkhite mayi”.",
"ja398:9.1": "“Mameva sutano attho,",
"ja398:9.2": "yathā bhāsasi māṇava;",
"ja398:9.3": "Mayā tvaṃ samanuññāto,",
"ja398:9.4": "sotthiṃ passāhi mātaraṃ.",
"ja398:10.1": "Khaggaṃ chattañca pātiñca,",
"ja398:10.2": "gacchamādāya māṇava;",
"ja398:10.3": "Sotthiṃ passatu te mātā,",
"ja398:10.4": "tvañca passāhi mātaraṃ”.",
"ja398:11.1": "“Evaṃ yakkha sukhī hohi,",
"ja398:11.2": "saha sabbehi ñātibhi;",
"ja398:11.3": "Dhanañca me adhigataṃ,",
"ja398:11.4": "rañño ca vacanaṃ katan”ti.",
"ja398:12.1": "Sutanujātakaṃ tatiyaṃ."
}