{
"ja158:1.1": "Jātaka",
"ja158:2.1": "Dukanipāta",
"ja158:3.1": "Daḷhavagga",
"ja158:4.1": "8. Suhanujātaka",
"ja158:5.1": "“Nayidaṃ visamasīlena,",
"ja158:5.2": "soṇena suhanū saha;",
"ja158:5.3": "Suhanūpi tādisoyeva,",
"ja158:5.4": "yo soṇassa sagocaro.",
"ja158:6.1": "Pakkhandinā pagabbhena,",
"ja158:6.2": "niccaṃ sandānakhādinā;",
"ja158:6.3": "Sameti pāpaṃ pāpena,",
"ja158:6.4": "sameti asatā asan”ti.",
"ja158:7.1": "Suhanujātakaṃ aṭṭhamaṃ."
}