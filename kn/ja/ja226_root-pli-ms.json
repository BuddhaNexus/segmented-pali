{
"ja226:1.1": "Jātaka",
"ja226:2.1": "Dukanipāta",
"ja226:3.1": "Kāsāvavagga",
"ja226:4.1": "6. Kosiyajātaka",
"ja226:5.1": "“Kāle nikkhamanā sādhu,",
"ja226:5.2": "nākāle sādhu nikkhamo;",
"ja226:5.3": "Akālena hi nikkhamma,",
"ja226:5.4": "ekakampi bahujjano;",
"ja226:5.5": "Na kiñci atthaṃ joteti,",
"ja226:5.6": "dhaṅkasenāva kosiyaṃ.",
"ja226:6.1": "Dhīro ca vidhividhānaññū,",
"ja226:6.2": "paresaṃ vivarānugū;",
"ja226:6.3": "Sabbāmitte vasīkatvā,",
"ja226:6.4": "kosiyova sukhī siyā”ti.",
"ja226:7.1": "Kosiyajātakaṃ chaṭṭhaṃ."
}