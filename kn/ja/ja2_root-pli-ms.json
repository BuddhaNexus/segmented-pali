{
"ja2:1.1": "Jātaka",
"ja2:2.1": "Ekakanipāta",
"ja2:3.1": "Apaṇṇakavagga",
"ja2:4.1": "2. Vaṇṇupathajātaka",
"ja2:5.1": "“Akilāsuno vaṇṇupathe khaṇantā,",
"ja2:5.2": "Udaṅgaṇe tattha papaṃ avinduṃ;",
"ja2:5.3": "Evaṃ munī viriyabalūpapanno,",
"ja2:5.4": "Akilāsu vinde hadayassa santin”ti.",
"ja2:6.1": "Vaṇṇupathajātakaṃ dutiyaṃ."
}