{
"ja399:1.1": "Jātaka",
"ja399:2.1": "Sattakanipāta",
"ja399:3.1": "Kukkuvagga",
"ja399:4.1": "4. Mātuposakagijjhajātaka",
"ja399:5.1": "“Te kathaṃ nu karissanti,",
"ja399:5.2": "vuddhā giridarīsayā;",
"ja399:5.3": "Ahaṃ baddhosmi pāsena,",
"ja399:5.4": "nilīyassa vasaṃ gato”.",
"ja399:6.1": "“Kiṃ gijjha paridevasi,",
"ja399:6.2": "kā nu te paridevanā;",
"ja399:6.3": "Na me suto vā diṭṭho vā,",
"ja399:6.4": "bhāsanto mānusiṃ dijo”.",
"ja399:7.1": "“Bharāmi mātāpitaro,",
"ja399:7.2": "vuddhe giridarīsaye;",
"ja399:7.3": "Te kathaṃ nu karissanti,",
"ja399:7.4": "ahaṃ vasaṃ gato tava”.",
"ja399:8.1": "“Yannu gijjho yojanasataṃ,",
"ja399:8.2": "kuṇapāni avekkhati;",
"ja399:8.3": "Kasmā jālañca pāsañca,",
"ja399:8.4": "āsajjāpi na bujjhasi”.",
"ja399:9.1": "“Yadā parābhavo hoti,",
"ja399:9.2": "poso jīvitasaṅkhaye;",
"ja399:9.3": "Atha jālañca pāsañca,",
"ja399:9.4": "āsajjāpi na bujjhati”.",
"ja399:10.1": "“Bharassu mātāpitaro,",
"ja399:10.2": "vuddhe giridarīsaye;",
"ja399:10.3": "Mayā tvaṃ samanuññāto,",
"ja399:10.4": "sotthiṃ passāhi ñātake”.",
"ja399:11.1": "“Evaṃ luddaka nandassu,",
"ja399:11.2": "saha sabbehi ñātibhi;",
"ja399:11.3": "Bharissaṃ mātāpitaro,",
"ja399:11.4": "vuddhe giridarīsaye”ti.",
"ja399:12.1": "Mātuposakagijjhajātakaṃ catutthaṃ."
}