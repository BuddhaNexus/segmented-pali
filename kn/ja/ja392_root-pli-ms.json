{
"ja392:1.1": "Jātaka",
"ja392:2.1": "Chakkanipāta",
"ja392:3.1": "Kharaputtavagga",
"ja392:4.1": "7. Siṅghapupphajātaka",
"ja392:5.1": "“Yametaṃ vārijaṃ pupphaṃ,",
"ja392:5.2": "Adinnaṃ upasiṅghasi;",
"ja392:5.3": "Ekaṅgametaṃ theyyānaṃ,",
"ja392:5.4": "Gandhathenosi mārisa”.",
"ja392:6.1": "“Na harāmi na bhañjāmi,",
"ja392:6.2": "ārā siṅghāmi vārijaṃ;",
"ja392:6.3": "Atha kena nu vaṇṇena,",
"ja392:6.4": "gandhathenoti vuccati.",
"ja392:7.1": "Yoyaṃ bhisāni khaṇati,",
"ja392:7.2": "puṇḍarīkāni bhañjati;",
"ja392:7.3": "Evaṃ ākiṇṇakammanto,",
"ja392:7.4": "kasmā eso na vuccati”.",
"ja392:8.1": "“Ākiṇṇaluddo puriso,",
"ja392:8.2": "dhāticelaṃva makkhito;",
"ja392:8.3": "Tasmiṃ me vacanaṃ natthi,",
"ja392:8.4": "tañcārahāmi vattave.",
"ja392:9.1": "Anaṅgaṇassa posassa,",
"ja392:9.2": "niccaṃ sucigavesino;",
"ja392:9.3": "Vālaggamattaṃ pāpassa,",
"ja392:9.4": "abbhāmattaṃva khāyati”.",
"ja392:10.1": "“Addhā maṃ yakkha jānāsi,",
"ja392:10.2": "atho maṃ anukampasi;",
"ja392:10.3": "Punapi yakkha vajjāsi,",
"ja392:10.4": "yadā passasi edisaṃ”.",
"ja392:11.1": "“Neva taṃ upajīvāmi,",
"ja392:11.2": "napi te bhatakāmhase;",
"ja392:11.3": "Tvameva bhikkhu jāneyya,",
"ja392:11.4": "yena gaccheyya suggatin”ti.",
"ja392:12.1": "Siṅghapupphajātakaṃ sattamaṃ."
}