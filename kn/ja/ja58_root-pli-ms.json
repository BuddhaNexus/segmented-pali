{
"ja58:1.1": "Jātaka",
"ja58:2.1": "Ekakanipāta",
"ja58:3.1": "Āsīsavagga",
"ja58:4.1": "8. Tayodhammajātaka",
"ja58:5.1": "“Yassa ete tayo dhammā,",
"ja58:5.2": "vānarinda yathā tava;",
"ja58:5.3": "Dakkhiyaṃ sūriyaṃ paññā,",
"ja58:5.4": "diṭṭhaṃ so ativattatī”ti.",
"ja58:6.1": "Tayodhammajātakaṃ aṭṭhamaṃ."
}