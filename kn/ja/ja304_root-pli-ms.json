{
"ja304:1.1": "Jātaka",
"ja304:2.1": "Catukkanipāta",
"ja304:3.1": "Kāliṅgavagga",
"ja304:4.1": "4. Daddarajātaka",
"ja304:5.1": "“Imāni maṃ daddara tāpayanti,",
"ja304:5.2": "Vācāduruttāni manussaloke;",
"ja304:5.3": "Maṇḍūkabhakkhā udakantasevī,",
"ja304:5.4": "Āsīvisaṃ maṃ avisā sapanti”.",
"ja304:6.1": "“Sakā raṭṭhā pabbājito,",
"ja304:6.2": "aññaṃ janapadaṃ gato;",
"ja304:6.3": "Mahantaṃ koṭṭhaṃ kayirātha,",
"ja304:6.4": "duruttānaṃ nidhetave.",
"ja304:7.1": "Yattha posaṃ na jānanti,",
"ja304:7.2": "jātiyā vinayena vā;",
"ja304:7.3": "Na tattha mānaṃ kayirātha,",
"ja304:7.4": "vasamaññātake jane.",
"ja304:8.1": "Videsavāsaṃ vasato,",
"ja304:8.2": "jātavedasamenapi;",
"ja304:8.3": "Khamitabbaṃ sapaññena,",
"ja304:8.4": "api dāsassa tajjitan”ti.",
"ja304:9.1": "Daddarajātakaṃ catutthaṃ."
}