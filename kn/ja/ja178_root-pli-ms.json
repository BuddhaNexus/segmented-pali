{
"ja178:1.1": "Jātaka",
"ja178:2.1": "Dukanipāta",
"ja178:3.1": "Kalyāṇavagga",
"ja178:4.1": "8. Kacchapajātaka",
"ja178:5.1": "“Janittaṃ me bhavittaṃ me,",
"ja178:5.2": "iti paṅke avassayiṃ;",
"ja178:5.3": "Taṃ maṃ paṅko ajjhabhavi,",
"ja178:5.4": "yathā dubbalakaṃ tathā;",
"ja178:5.5": "Taṃ taṃ vadāmi bhaggava,",
"ja178:5.6": "suṇohi vacanaṃ mama”.",
"ja178:6.1": "“Gāme vā yadi vā raññe,",
"ja178:6.2": "sukhaṃ yatrādhigacchati;",
"ja178:6.3": "Taṃ janittaṃ bhavittañca,",
"ja178:6.4": "purisassa pajānato;",
"ja178:6.5": "Yamhi jīve tamhi gacche,",
"ja178:6.6": "na niketahato siyā”ti.",
"ja178:7.1": "Kacchapajātakaṃ aṭṭhamaṃ."
}