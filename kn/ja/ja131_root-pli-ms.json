{
"ja131:1.1": "Jātaka",
"ja131:2.1": "Ekakanipāta",
"ja131:3.1": "Asampadānavagga",
"ja131:4.1": "1. Asampadānajātaka",
"ja131:5.1": "“Asampadānenitarītarassa,",
"ja131:5.2": "Bālassa mittāni kalī bhavanti;",
"ja131:5.3": "Tasmā harāmi bhusaṃ aḍḍhamānaṃ,",
"ja131:5.4": "Mā me mitti jīyittha sassatāyan”ti.",
"ja131:6.1": "Asampadānajātakaṃ paṭhamaṃ."
}