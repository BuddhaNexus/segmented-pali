{
"ja23:1.1": "Jātaka",
"ja23:2.1": "Ekakanipāta",
"ja23:3.1": "Kuruṅgavagga",
"ja23:4.1": "3. Bhojājānīyajātaka",
"ja23:5.1": "“Api passena semāno,",
"ja23:5.2": "sallebhi sallalīkato;",
"ja23:5.3": "Seyyova vaḷavā bhojjo,",
"ja23:5.4": "yuñja maññeva sārathī”ti.",
"ja23:6.1": "Bhojājānīyajātakaṃ tatiyaṃ."
}