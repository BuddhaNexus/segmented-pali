{
"ja212:1.1": "Jātaka",
"ja212:2.1": "Dukanipāta",
"ja212:3.1": "Bīraṇathambhavagga",
"ja212:4.1": "2. Ucchiṭṭhabhattajātaka",
"ja212:5.1": "“Añño uparimo vaṇṇo,",
"ja212:5.2": "añño vaṇṇo ca heṭṭhimo;",
"ja212:5.3": "Brāhmaṇī tveva pucchāmi,",
"ja212:5.4": "kiṃ heṭṭhā kiñca uppari”.",
"ja212:6.1": "“Ahaṃ naṭosmi bhaddante,",
"ja212:6.2": "bhikkhakosmi idhāgato;",
"ja212:6.3": "Ayañhi koṭṭhamotiṇṇo,",
"ja212:6.4": "ayaṃ so yaṃ gavesasī”ti.",
"ja212:7.1": "Ucchiṭṭhabhattajātakaṃ dutiyaṃ."
}