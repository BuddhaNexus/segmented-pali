{
"ja170:1.1": "Jātaka",
"ja170:2.1": "Dukanipāta",
"ja170:3.1": "Santhavavagga",
"ja170:4.1": "10. Kakaṇṭakajātaka",
"ja170:5.1": "“Nāyaṃ pure uṇṇamati,",
"ja170:5.2": "toraṇagge kakaṇṭako;",
"ja170:5.3": "Mahosadha vijānāhi,",
"ja170:5.4": "kena thaddho kakaṇṭako”.",
"ja170:6.1": "“Aladdhapubbaṃ laddhāna,",
"ja170:6.2": "aḍḍhamāsaṃ kakaṇṭako;",
"ja170:6.3": "Atimaññati rājānaṃ,",
"ja170:6.4": "vedehaṃ mithilaggahan”ti.",
"ja170:7.1": "Kakaṇṭakajātakaṃ dasamaṃ.",
"ja170:7.2": "Santhavavaggo dutiyo.",
"ja170:8.1": "Tassuddānaṃ",
"ja170:9.1": "Atha indasamāna sapaṇṇakuṭi,",
"ja170:9.2": "Susimuttama gijjha jalābujako;",
"ja170:9.3": "Upasāḷaka bhikkhu salāpavaro,",
"ja170:9.4": "Atha mettavaro dasapuṇṇamatīti."
}