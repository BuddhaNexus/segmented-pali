{
"ja31:1.1": "Jātaka",
"ja31:2.1": "Ekakanipāta",
"ja31:3.1": "Kulāvakavagga",
"ja31:4.1": "1. Kulāvakajātaka",
"ja31:5.1": "“Kulāvakā mātali simbalismiṃ,",
"ja31:5.2": "Īsāmukhena parivajjayassu;",
"ja31:5.3": "Kāmaṃ cajāma asuresu pāṇaṃ,",
"ja31:5.4": "Māme dijā vikkulāvā ahesun”ti.",
"ja31:6.1": "Kulāvakajātakaṃ paṭhamaṃ."
}