{
"ja164:1.1": "Jātaka",
"ja164:2.1": "Dukanipāta",
"ja164:3.1": "Santhavavagga",
"ja164:4.1": "4. Gijjhajātaka",
"ja164:5.1": "“Yannu gijjho yojanasataṃ,",
"ja164:5.2": "kuṇapāni avekkhati;",
"ja164:5.3": "Kasmā jālañca pāsañca,",
"ja164:5.4": "āsajjāpi na bujjhasi”.",
"ja164:6.1": "“Yadā parābhavo hoti,",
"ja164:6.2": "poso jīvitasaṅkhaye;",
"ja164:6.3": "Atha jālañca pāsañca,",
"ja164:6.4": "āsajjāpi na bujjhatī”ti.",
"ja164:7.1": "Gijjhajātakaṃ catutthaṃ."
}