{
"ja20:1.1": "Jātaka",
"ja20:2.1": "Ekakanipāta",
"ja20:3.1": "Sīlavagga",
"ja20:4.1": "10. Naḷapānajātaka",
"ja20:5.1": "“Disvā padamanuttiṇṇaṃ,",
"ja20:5.2": "Disvānotaritaṃ padaṃ;",
"ja20:5.3": "Naḷena vāriṃ pissāma,",
"ja20:5.4": "Neva maṃ tvaṃ vadhissasī”ti.",
"ja20:6.1": "Naḷapānajātakaṃ dasamaṃ.",
"ja20:6.2": "Sīlavaggo dutiyo.",
"ja20:7.1": "Tassuddānaṃ",
"ja20:8.1": "Atha lakkhaṇa sākha dhiratthu puna,",
"ja20:8.2": "Na kiratthi rasehi kharādiyā;",
"ja20:8.3": "Atibhoti vara māluta pāṇa,",
"ja20:8.4": "Muccena naḷaavhayanena bhavanti dasāti."
}