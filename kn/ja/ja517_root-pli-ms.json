{
"ja517:1.1": "Jātaka",
"ja517:2.1": "Tiṃsanipāta",
"ja517:3.1": "Kiṃchandavagga",
"ja517:4.1": "7. Dakarakkhasajātaka",
"ja517:5.1": "“Sace vo vuyhamānānaṃ,",
"ja517:5.2": "sattannaṃ udakaṇṇave;",
"ja517:5.3": "Manussabalimesāno,",
"ja517:5.4": "nāvaṃ gaṇheyya rakkhaso;",
"ja517:5.5": "Anupubbaṃ kathaṃ datvā,",
"ja517:5.6": "muñcesi dakarakkhasā”.",
"ja517:6.1": "“Mātaraṃ paṭhamaṃ dajjaṃ,",
"ja517:6.2": "Bhariyaṃ datvāna bhātaraṃ;",
"ja517:6.3": "Tato sahāyaṃ datvāna,",
"ja517:6.4": "Pañcamaṃ dajjaṃ brāhmaṇaṃ;",
"ja517:6.5": "Chaṭṭhāhaṃ dajjamattānaṃ,",
"ja517:6.6": "Neva dajjaṃ mahosadhaṃ”.",
"ja517:7.1": "“Posetā te janettī ca,",
"ja517:7.2": "dīgharattānukampikā;",
"ja517:7.3": "Chabbhī tayi padussati,",
"ja517:7.4": "paṇḍitā atthadassinī;",
"ja517:7.5": "Aññaṃ upanisaṃ katvā,",
"ja517:7.6": "vadhā taṃ parimocayi.",
"ja517:8.1": "Taṃ tādisiṃ pāṇadadiṃ,",
"ja517:8.2": "orasaṃ gabbhadhāriniṃ;",
"ja517:8.3": "Mātaraṃ kena dosena,",
"ja517:8.4": "dajjāsi dakarakkhino”.",
"ja517:9.1": "“Daharā viyalaṅkāraṃ,",
"ja517:9.2": "dhāreti apiḷandhanaṃ;",
"ja517:9.3": "Dovārike anīkaṭṭhe,",
"ja517:9.4": "ativelaṃ pajagghati.",
"ja517:10.1": "Athopi paṭirājūnaṃ,",
"ja517:10.2": "sayaṃ dūtāni sāsati;",
"ja517:10.3": "Mātaraṃ tena dosena,",
"ja517:10.4": "dajjāhaṃ dakarakkhino”.",
"ja517:11.1": "“Itthigumbassa pavarā,",
"ja517:11.2": "accantaṃ piyabhāṇinī;",
"ja517:11.3": "Anuggatā sīlavatī,",
"ja517:11.4": "chāyāva anapāyinī.",
"ja517:12.1": "Akkodhanā puññavatī,",
"ja517:12.2": "paṇḍitā atthadassinī;",
"ja517:12.3": "Ubbariṃ kena dosena,",
"ja517:12.4": "dajjāsi dakarakkhino”.",
"ja517:13.1": "“Khiḍḍāratisamāpannaṃ,",
"ja517:13.2": "anatthavasamāgataṃ;",
"ja517:13.3": "Sā maṃ sakāna puttānaṃ,",
"ja517:13.4": "ayācaṃ yācate dhanaṃ.",
"ja517:14.1": "Sohaṃ dadāmi sāratto,",
"ja517:14.2": "bahuṃ uccāvacaṃ dhanaṃ;",
"ja517:14.3": "Suduccajaṃ cajitvāna,",
"ja517:14.4": "pacchā socāmi dummano;",
"ja517:14.5": "Ubbariṃ tena dosena,",
"ja517:14.6": "dajjāhaṃ dakarakkhino”.",
"ja517:15.1": "“Yenocitā janapadā,",
"ja517:15.2": "ānītā ca paṭiggahaṃ;",
"ja517:15.3": "Ābhataṃ pararajjebhi,",
"ja517:15.4": "abhiṭṭhāya bahuṃ dhanaṃ.",
"ja517:16.1": "Dhanuggahānaṃ pavaraṃ,",
"ja517:16.2": "sūraṃ tikhiṇamantinaṃ;",
"ja517:16.3": "Bhātaraṃ kena dosena,",
"ja517:16.4": "dajjāsi dakarakkhino”.",
"ja517:17.1": "“Yenocitā janapadā,",
"ja517:17.2": "ānītā ca paṭiggahaṃ;",
"ja517:17.3": "Ābhataṃ pararajjebhi,",
"ja517:17.4": "abhiṭṭhāya bahuṃ dhanaṃ.",
"ja517:18.1": "Dhanuggahānaṃ pavaro,",
"ja517:18.2": "sūro tikhiṇamanti ca;",
"ja517:18.3": "Mayāyaṃ sukhito rājā,",
"ja517:18.4": "atimaññati dārako.",
"ja517:19.1": "Upaṭṭhānampi me ayye,",
"ja517:19.2": "na so eti yathā pure;",
"ja517:19.3": "Bhātaraṃ tena dosena,",
"ja517:19.4": "dajjāhaṃ dakarakkhino”.",
"ja517:20.1": "“Ekarattena ubhayo,",
"ja517:20.2": "tvañceva dhanusekha ca;",
"ja517:20.3": "Ubho jātettha pañcālā,",
"ja517:20.4": "sahāyā susamāvayā.",
"ja517:21.1": "Cariyā taṃ anubandhittho,",
"ja517:21.2": "ekadukkhasukho tava;",
"ja517:21.3": "Ussukko te divārattiṃ,",
"ja517:21.4": "sabbakiccesu byāvaṭo;",
"ja517:21.5": "Sahāyaṃ kena dosena,",
"ja517:21.6": "dajjāsi dakarakkhino”.",
"ja517:22.1": "“Cariyā maṃ ayaṃ ayye,",
"ja517:22.2": "pajagghittho mayā saha;",
"ja517:22.3": "Ajjāpi tena vaṇṇena,",
"ja517:22.4": "ativelaṃ pajagghati.",
"ja517:23.1": "Ubbariyāpihaṃ ayye,",
"ja517:23.2": "mantayāmi rahogato;",
"ja517:23.3": "Anāmanto pavisati,",
"ja517:23.4": "pubbe appaṭivedito.",
"ja517:24.1": "Laddhadvāro katokāso,",
"ja517:24.2": "ahirikaṃ anādaraṃ;",
"ja517:24.3": "Sahāyaṃ tena dosena,",
"ja517:24.4": "dajjāhaṃ dakarakkhino”.",
"ja517:25.1": "“Kusalo sabbanimittānaṃ,",
"ja517:25.2": "rutaññū āgatāgamo;",
"ja517:25.3": "Uppāte supine yutto,",
"ja517:25.4": "niyyāne ca pavesane.",
"ja517:26.1": "Paṭṭho bhūmantalikkhasmiṃ,",
"ja517:26.2": "Nakkhattapadakovido;",
"ja517:26.3": "Brāhmaṇaṃ kena dosena,",
"ja517:26.4": "Dajjāsi dakarakkhino”.",
"ja517:27.1": "“Parisāyampi me ayye,",
"ja517:27.2": "ummīlitvā udikkhati;",
"ja517:27.3": "Tasmā accabhamuṃ luddaṃ,",
"ja517:27.4": "dajjāhaṃ dakarakkhino”.",
"ja517:28.1": "“Sasamuddapariyāyaṃ,",
"ja517:28.2": "mahiṃ sāgarakuṇḍalaṃ;",
"ja517:28.3": "Vasundharaṃ āvasasi,",
"ja517:28.4": "amaccaparivārito.",
"ja517:29.1": "Cāturanto mahāraṭṭho,",
"ja517:29.2": "vijitāvī mahabbalo;",
"ja517:29.3": "Pathabyā ekarājāsi,",
"ja517:29.4": "yaso te vipulaṃ gato.",
"ja517:30.1": "Soḷasitthisahassāni,",
"ja517:30.2": "āmuttamaṇikuṇḍalā;",
"ja517:30.3": "Nānājanapadā nārī,",
"ja517:30.4": "devakaññūpamā subhā.",
"ja517:31.1": "Evaṃ sabbaṅgasampannaṃ,",
"ja517:31.2": "sabbakāmasamiddhinaṃ;",
"ja517:31.3": "Sukhitānaṃ piyaṃ dīghaṃ,",
"ja517:31.4": "jīvitaṃ āhu khattiya.",
"ja517:32.1": "Atha tvaṃ kena vaṇṇena,",
"ja517:32.2": "kena vā pana hetunā;",
"ja517:32.3": "Paṇḍitaṃ anurakkhanto,",
"ja517:32.4": "pāṇaṃ cajasi duccajaṃ”.",
"ja517:33.1": "“Yatopi āgato ayye,",
"ja517:33.2": "mama hatthaṃ mahosadho;",
"ja517:33.3": "Nābhijānāmi dhīrassa,",
"ja517:33.4": "anumattampi dukkaṭaṃ.",
"ja517:34.1": "Sace ca kismici kāle,",
"ja517:34.2": "maraṇaṃ me pure siyā;",
"ja517:34.3": "So me putte paputte ca,",
"ja517:34.4": "sukhāpeyya mahosadho.",
"ja517:35.1": "Anāgataṃ paccuppannaṃ,",
"ja517:35.2": "sabbamatthampi passati;",
"ja517:35.3": "Anāparādhakammantaṃ,",
"ja517:35.4": "na dajjaṃ dakarakkhino”.",
"ja517:36.1": "“Idaṃ suṇātha pañcālā,",
"ja517:36.2": "cūḷaneyyassa bhāsitaṃ;",
"ja517:36.3": "Paṇḍitaṃ anurakkhanto,",
"ja517:36.4": "pāṇaṃ cajati duccajaṃ.",
"ja517:37.1": "Mātu bhariyāya bhātucca,",
"ja517:37.2": "sakhino brāhmaṇassa ca;",
"ja517:37.3": "Attano cāpi pañcālo,",
"ja517:37.4": "channaṃ cajati jīvitaṃ.",
"ja517:38.1": "Evaṃ mahatthikā paññā,",
"ja517:38.2": "nipuṇā sādhucintinī;",
"ja517:38.3": "Diṭṭhadhammahitatthāya,",
"ja517:38.4": "samparāyasukhāya cā”ti.",
"ja517:39.1": "Dakarakkhasajātakaṃ sattamaṃ."
}