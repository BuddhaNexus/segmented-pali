{
"ja370:1.1": "Jātaka",
"ja370:2.1": "Pañcakanipāta",
"ja370:3.1": "Vaṇṇārohavagga",
"ja370:4.1": "10. Palāsajātaka",
"ja370:5.1": "“Haṃso palāsamavaca,",
"ja370:5.2": "nigrodho samma jāyati;",
"ja370:5.3": "Aṅkasmiṃ te nisinnova,",
"ja370:5.4": "so te mammāni checchati”.",
"ja370:6.1": "“Vaḍḍhatāmeva nigrodho,",
"ja370:6.2": "patiṭṭhassa bhavāmahaṃ;",
"ja370:6.3": "Yathā pitā ca mātā ca,",
"ja370:6.4": "evaṃ me so bhavissati”.",
"ja370:7.1": "“Yaṃ tvaṃ aṅkasmiṃ vaḍḍhesi,",
"ja370:7.2": "khīrarukkhaṃ bhayānakaṃ;",
"ja370:7.3": "Āmanta kho taṃ gacchāma,",
"ja370:7.4": "vuḍḍhi massa na ruccati”.",
"ja370:8.1": "“Idāni kho maṃ bhāyeti,",
"ja370:8.2": "mahānerunidassanaṃ;",
"ja370:8.3": "Haṃsassa anabhiññāya,",
"ja370:8.4": "mahā me bhayamāgataṃ”.",
"ja370:9.1": "“Na tassa vuḍḍhi kusalappasatthā,",
"ja370:9.2": "Yo vaḍḍhamāno ghasate patiṭṭhaṃ;",
"ja370:9.3": "Tassūparodhaṃ parisaṅkamāno,",
"ja370:9.4": "Patārayī mūlavadhāya dhīro”ti.",
"ja370:10.1": "Palāsajātakaṃ dasamaṃ.",
"ja370:10.2": "Vaṇṇārohavaggo dutiyo."
}