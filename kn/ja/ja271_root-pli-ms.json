{
"ja271:1.1": "Jātaka",
"ja271:2.1": "Tikanipāta",
"ja271:3.1": "Udapānavagga",
"ja271:4.1": "1. Udapānadūsakajātaka",
"ja271:5.1": "“Āraññikassa isino,",
"ja271:5.2": "cirarattatapassino;",
"ja271:5.3": "Kicchākataṃ udapānaṃ,",
"ja271:5.4": "kathaṃ samma avāhayi”.",
"ja271:6.1": "“Esa dhammo siṅgālānaṃ,",
"ja271:6.2": "yaṃ pitvā ohadāmase;",
"ja271:6.3": "Pitupitāmahaṃ dhammo,",
"ja271:6.4": "na taṃ ujjhātumarahasi”.",
"ja271:7.1": "“Yesaṃ vo ediso dhammo,",
"ja271:7.2": "Adhammo pana kīdiso;",
"ja271:7.3": "Mā vo dhammaṃ adhammaṃ vā,",
"ja271:7.4": "Addasāma kudācanan”ti.",
"ja271:8.1": "Udapānadūsakajātakaṃ paṭhamaṃ."
}