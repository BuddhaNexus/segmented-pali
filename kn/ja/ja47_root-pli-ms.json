{
"ja47:1.1": "Jātaka",
"ja47:2.1": "Ekakanipāta",
"ja47:3.1": "Atthakāmavagga",
"ja47:4.1": "7. Vāruṇidūsakajātaka",
"ja47:5.1": "“Na ve anatthakusalena,",
"ja47:5.2": "atthacariyā sukhāvahā;",
"ja47:5.3": "Hāpeti atthaṃ dummedho,",
"ja47:5.4": "koṇḍañño vāruṇiṃ yathā”ti.",
"ja47:6.1": "Vāruṇidūsakajātakaṃ sattamaṃ."
}