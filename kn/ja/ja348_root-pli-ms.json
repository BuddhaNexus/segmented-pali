{
"ja348:1.1": "Jātaka",
"ja348:2.1": "Catukkanipāta",
"ja348:3.1": "Cūḷakuṇālavagga",
"ja348:4.1": "8. Araññajātaka",
"ja348:5.1": "“Araññā gāmamāgamma,",
"ja348:5.2": "kiṃsīlaṃ kiṃvataṃ ahaṃ;",
"ja348:5.3": "Purisaṃ tāta seveyyaṃ,",
"ja348:5.4": "taṃ me akkhāhi pucchito”.",
"ja348:6.1": "“Yo taṃ vissāsaye tāta,",
"ja348:6.2": "vissāsañca khameyya te;",
"ja348:6.3": "Sussūsī ca titikkhī ca,",
"ja348:6.4": "taṃ bhajehi ito gato.",
"ja348:7.1": "Yassa kāyena vācāya,",
"ja348:7.2": "manasā natthi dukkaṭaṃ;",
"ja348:7.3": "Urasīva patiṭṭhāya,",
"ja348:7.4": "taṃ bhajehi ito gato.",
"ja348:8.1": "Haliddirāgaṃ kapicittaṃ,",
"ja348:8.2": "purisaṃ rāgavirāginaṃ;",
"ja348:8.3": "Tādisaṃ tāta mā sevi,",
"ja348:8.4": "nimmanussampi ce siyā”ti.",
"ja348:9.1": "Araññajātakaṃ aṭṭhamaṃ."
}