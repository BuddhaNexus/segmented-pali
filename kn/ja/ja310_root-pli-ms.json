{
"ja310:1.1": "Jātaka",
"ja310:2.1": "Catukkanipāta",
"ja310:3.1": "Kāliṅgavagga",
"ja310:4.1": "10. Seyyajātaka",
"ja310:5.1": "“Sasamuddapariyāyaṃ,",
"ja310:5.2": "mahiṃ sāgarakuṇḍalaṃ;",
"ja310:5.3": "Na icche saha nindāya,",
"ja310:5.4": "evaṃ seyya vijānahi.",
"ja310:6.1": "Dhiratthu taṃ yasalābhaṃ,",
"ja310:6.2": "dhanalābhañca brāhmaṇa;",
"ja310:6.3": "Yā vutti vinipātena,",
"ja310:6.4": "adhammacaraṇena vā.",
"ja310:7.1": "Api ce pattamādāya,",
"ja310:7.2": "anagāro paribbaje;",
"ja310:7.3": "Sāyeva jīvikā seyyo,",
"ja310:7.4": "yā cādhammena esanā.",
"ja310:8.1": "Api ce pattamādāya,",
"ja310:8.2": "anagāro paribbaje;",
"ja310:8.3": "Aññaṃ ahiṃsayaṃ loke,",
"ja310:8.4": "api rajjena taṃ varan”ti.",
"ja310:9.1": "Seyyajātakaṃ dasamaṃ.",
"ja310:9.2": "Kāliṅgavaggo paṭhamo.",
"ja310:10.1": "Tassuddānaṃ",
"ja310:11.1": "Vivarañca adeyya samiddhavaraṃ,",
"ja310:11.2": "Atha daddara pāpamahātiraho;",
"ja310:11.3": "Atha koli palāsavarañca kara,",
"ja310:11.4": "Carimaṃ sasamuddavarena dasāti."
}