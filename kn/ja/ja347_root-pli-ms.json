{
"ja347:1.1": "Jātaka",
"ja347:2.1": "Catukkanipāta",
"ja347:3.1": "Cūḷakuṇālavagga",
"ja347:4.1": "7. Ayakūṭajātaka",
"ja347:5.1": "“Sabbāyasaṃ kūṭamatippamāṇaṃ,",
"ja347:5.2": "Paggayha yo tiṭṭhasi antalikkhe;",
"ja347:5.3": "Rakkhāya me tvaṃ vihito nusajja,",
"ja347:5.4": "Udāhu me cetayase vadhāya”.",
"ja347:6.1": "“Dūto ahaṃ rājidha rakkhasānaṃ,",
"ja347:6.2": "Vadhāya tuyhaṃ pahitohamasmi;",
"ja347:6.3": "Indo ca taṃ rakkhati devarājā,",
"ja347:6.4": "Tenuttamaṅgaṃ na te phālayāmi”.",
"ja347:7.1": "“Sace ca maṃ rakkhati devarājā,",
"ja347:7.2": "Devānamindo maghavā sujampati;",
"ja347:7.3": "Kāmaṃ pisācā vinadantu sabbe,",
"ja347:7.4": "Na santase rakkhasiyā pajāya.",
"ja347:8.1": "Kāmaṃ kandantu kumbhaṇḍā,",
"ja347:8.2": "Sabbe paṃsupisācakā;",
"ja347:8.3": "Nālaṃ pisācā yuddhāya,",
"ja347:8.4": "Mahatī sā vibhiṃsikā”ti.",
"ja347:9.1": "Ayakūṭajātakaṃ sattamaṃ."
}