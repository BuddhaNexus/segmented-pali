{
"ja219:1.1": "Jātaka",
"ja219:2.1": "Dukanipāta",
"ja219:3.1": "Bīraṇathambhavagga",
"ja219:4.1": "9. Garahitajātaka",
"ja219:5.1": "“Hiraññaṃ me suvaṇṇaṃ me,",
"ja219:5.2": "esā rattiṃ divā kathā;",
"ja219:5.3": "Dummedhānaṃ manussānaṃ,",
"ja219:5.4": "ariyadhammaṃ apassataṃ.",
"ja219:6.1": "Dve dve gahapatayo gehe,",
"ja219:6.2": "eko tattha amassuko;",
"ja219:6.3": "Lambatthano veṇikato,",
"ja219:6.4": "atho aṅkitakaṇṇako;",
"ja219:6.5": "Kīto dhanena bahunā,",
"ja219:6.6": "so taṃ vitudate janan”ti.",
"ja219:7.1": "Garahitajātakaṃ navamaṃ."
}