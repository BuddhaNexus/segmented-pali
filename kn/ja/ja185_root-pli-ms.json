{
"ja185:1.1": "Jātaka",
"ja185:2.1": "Dukanipāta",
"ja185:3.1": "Asadisavagga",
"ja185:4.1": "5. Anabhiratijātaka",
"ja185:5.1": "“Yathodake āvile appasanne,",
"ja185:5.2": "Na passati sippikasambukañca;",
"ja185:5.3": "Sakkharaṃ vālukaṃ macchagumbaṃ,",
"ja185:5.4": "Evaṃ āvilamhi citte;",
"ja185:5.5": "Na passati attadatthaṃ paratthaṃ.",
"ja185:6.1": "Yathodake acche vippasanne,",
"ja185:6.2": "So passati sippikasambukañca;",
"ja185:6.3": "Sakkharaṃ vālukaṃ macchagumbaṃ,",
"ja185:6.4": "Evaṃ anāvilamhi citte;",
"ja185:6.5": "So passati attadatthaṃ paratthan”ti.",
"ja185:7.1": "Anabhiratijātakaṃ pañcamaṃ."
}