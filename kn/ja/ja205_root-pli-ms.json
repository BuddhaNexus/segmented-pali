{
"ja205:1.1": "Jātaka",
"ja205:2.1": "Dukanipāta",
"ja205:3.1": "Nataṃdaḷhavagga",
"ja205:4.1": "5. Gaṅgeyyajātaka",
"ja205:5.1": "“Sobhati maccho gaṅgeyyo,",
"ja205:5.2": "atho sobhati yāmuno;",
"ja205:5.3": "Catuppadoyaṃ puriso,",
"ja205:5.4": "nigrodhaparimaṇḍalo;",
"ja205:5.5": "Īsakāyatagīvo ca,",
"ja205:5.6": "sabbeva atirocati”.",
"ja205:6.1": "“Yaṃ pucchito na taṃ akkhāsi,",
"ja205:6.2": "Aññaṃ akkhāsi pucchito;",
"ja205:6.3": "Attappasaṃsako poso,",
"ja205:6.4": "Nāyaṃ asmāka ruccatī”ti.",
"ja205:7.1": "Gaṅgeyyajātakaṃ pañcamaṃ."
}