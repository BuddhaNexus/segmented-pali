{
"ja188:1.1": "Jātaka",
"ja188:2.1": "Dukanipāta",
"ja188:3.1": "Asadisavagga",
"ja188:4.1": "8. Sīhakotthujātaka",
"ja188:5.1": "“Sīhaṅgulī sīhanakho,",
"ja188:5.2": "sīhapādapatiṭṭhito;",
"ja188:5.3": "So sīho sīhasaṃghamhi,",
"ja188:5.4": "eko nadati aññathā”.",
"ja188:6.1": "“Mā tvaṃ nadi rājaputta,",
"ja188:6.2": "appasaddo vane vasa;",
"ja188:6.3": "Sarena kho taṃ jāneyyuṃ,",
"ja188:6.4": "na hi te pettiko saro”ti.",
"ja188:7.1": "Sīhakotthujātakaṃ aṭṭhamaṃ."
}