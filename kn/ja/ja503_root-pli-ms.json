{
"ja503:1.1": "Jātaka",
"ja503:2.1": "Vīsatinipāta",
"ja503:3.1": "Mātaṅgavagga",
"ja503:4.1": "7. Sattigumbajātaka",
"ja503:5.1": "Migaluddo mahārājā,",
"ja503:5.2": "pañcālānaṃ rathesabho;",
"ja503:5.3": "Nikkhanto saha senāya,",
"ja503:5.4": "ogaṇo vanamāgamā.",
"ja503:6.1": "Tatthaddasā araññasmiṃ,",
"ja503:6.2": "takkarānaṃ kuṭiṃ kataṃ;",
"ja503:6.3": "Tassā kuṭiyā nikkhamma,",
"ja503:6.4": "suvo luddāni bhāsati.",
"ja503:7.1": "“Sampannavāhano poso,",
"ja503:7.2": "yuvā sammaṭṭhakuṇḍalo;",
"ja503:7.3": "Sobhati lohituṇhīso,",
"ja503:7.4": "divā sūriyova bhāsati.",
"ja503:8.1": "Majjhanhike sampatike,",
"ja503:8.2": "sutto rājā sasārathi;",
"ja503:8.3": "Handassābharaṇaṃ sabbaṃ,",
"ja503:8.4": "gaṇhāma sāhasā mayaṃ.",
"ja503:9.1": "Nisīthepi raho dāni,",
"ja503:9.2": "Sutto rājā sasārathi;",
"ja503:9.3": "Ādāya vatthaṃ maṇikuṇḍalañca,",
"ja503:9.4": "Hantvāna sākhāhi avattharāma”.",
"ja503:10.1": "“Kiṃ nu ummattarūpova,",
"ja503:10.2": "sattigumba pabhāsasi;",
"ja503:10.3": "Durāsadā hi rājāno,",
"ja503:10.4": "aggi pajjalito yathā”.",
"ja503:11.1": "“Atha tvaṃ patikolamba,",
"ja503:11.2": "matto thullāni gajjasi;",
"ja503:11.3": "Mātari mayhaṃ naggāya,",
"ja503:11.4": "kiṃ nu tvaṃ vijigucchase”.",
"ja503:12.1": "“Uṭṭhehi samma taramāno,",
"ja503:12.2": "rathaṃ yojehi sārathi;",
"ja503:12.3": "Sakuṇo me na ruccati,",
"ja503:12.4": "aññaṃ gacchāma assamaṃ”.",
"ja503:13.1": "“Yutto ratho mahārāja,",
"ja503:13.2": "yutto ca balavāhano;",
"ja503:13.3": "Adhitiṭṭha mahārāja,",
"ja503:13.4": "aññaṃ gacchāma assamaṃ”.",
"ja503:14.1": "“Ko numeva gatā sabbe,",
"ja503:14.2": "ye asmiṃ paricārakā;",
"ja503:14.3": "Esa gacchati pañcālo,",
"ja503:14.4": "mutto tesaṃ adassanā.",
"ja503:15.1": "Kodaṇḍakāni gaṇhatha,",
"ja503:15.2": "sattiyo tomarāni ca;",
"ja503:15.3": "Esa gacchati pañcālo,",
"ja503:15.4": "mā vo muñcittha jīvataṃ”.",
"ja503:16.1": "Athāparo paṭinandittha,",
"ja503:16.2": "suvo lohitatuṇḍako;",
"ja503:16.3": "“Svāgataṃ te mahārāja,",
"ja503:16.4": "atho te adurāgataṃ;",
"ja503:16.5": "Issarosi anuppatto,",
"ja503:16.6": "yaṃ idhatthi pavedaya.",
"ja503:17.1": "Tindukāni piyālāni,",
"ja503:17.2": "madhuke kāsumāriyo;",
"ja503:17.3": "Phalāni khuddakappāni,",
"ja503:17.4": "bhuñja rāja varaṃ varaṃ.",
"ja503:18.1": "Idampi pānīyaṃ sītaṃ,",
"ja503:18.2": "ābhataṃ girigabbharā;",
"ja503:18.3": "Tato piva mahārāja,",
"ja503:18.4": "sace tvaṃ abhikaṅkhasi.",
"ja503:19.1": "Araññaṃ uñchāya gatā,",
"ja503:19.2": "ye asmiṃ paricārakā;",
"ja503:19.3": "Sayaṃ uṭṭhāya gaṇhavho,",
"ja503:19.4": "hatthā me natthi dātave”.",
"ja503:20.1": "“Bhaddako vatayaṃ pakkhī,",
"ja503:20.2": "dijo paramadhammiko;",
"ja503:20.3": "Atheso itaro pakkhī,",
"ja503:20.4": "suvo luddāni bhāsati.",
"ja503:21.1": "‘Etaṃ hanatha bandhatha,",
"ja503:21.2": "Mā vo muñcittha jīvataṃ’;",
"ja503:21.3": "Iccevaṃ vilapantassa,",
"ja503:21.4": "Sotthiṃ pattosmi assamaṃ”.",
"ja503:22.1": "“Bhātarosma mahārāja,",
"ja503:22.2": "sodariyā ekamātukā;",
"ja503:22.3": "Ekarukkhasmiṃ saṃvaḍḍhā,",
"ja503:22.4": "nānākhettagatā ubho.",
"ja503:23.1": "Sattigumbo ca corānaṃ,",
"ja503:23.2": "ahañca isīnaṃ idha;",
"ja503:23.3": "Asataṃ so, sataṃ ahaṃ,",
"ja503:23.4": "tena dhammena no vinā”.",
"ja503:24.1": "“Tattha vadho ca bandho ca,",
"ja503:24.2": "nikatī vañcanāni ca;",
"ja503:24.3": "Ālopā sāhasākārā,",
"ja503:24.4": "tāni so tattha sikkhati.",
"ja503:25.1": "Idha saccañca dhammo ca,",
"ja503:25.2": "Ahiṃsā saṃyamo damo;",
"ja503:25.3": "Āsanūdakadāyīnaṃ,",
"ja503:25.4": "Aṅke vaddhosmi bhāradha”.",
"ja503:26.1": "“Yaṃ yañhi rāja bhajati,",
"ja503:26.2": "santaṃ vā yadi vā asaṃ;",
"ja503:26.3": "Sīlavantaṃ visīlaṃ vā,",
"ja503:26.4": "vasaṃ tasseva gacchati.",
"ja503:27.1": "Yādisaṃ kurute mittaṃ,",
"ja503:27.2": "yādisaṃ cūpasevati;",
"ja503:27.3": "Sopi tādisako hoti,",
"ja503:27.4": "sahavāso hi tādiso.",
"ja503:28.1": "Sevamāno sevamānaṃ,",
"ja503:28.2": "samphuṭṭho samphusaṃ paraṃ;",
"ja503:28.3": "Saro diddho kalāpaṃva,",
"ja503:28.4": "alittamupalimpati;",
"ja503:28.5": "Upalepabhayā dhīro,",
"ja503:28.6": "neva pāpasakhā siyā.",
"ja503:29.1": "Pūtimacchaṃ kusaggena,",
"ja503:29.2": "yo naro upanayhati;",
"ja503:29.3": "Kusāpi pūti vāyanti,",
"ja503:29.4": "evaṃ bālūpasevanā.",
"ja503:30.1": "Tagarañca palāsena,",
"ja503:30.2": "yo naro upanayhati;",
"ja503:30.3": "Pattāpi surabhi vāyanti,",
"ja503:30.4": "evaṃ dhīrūpasevanā.",
"ja503:31.1": "Tasmā pattapuṭasseva,",
"ja503:31.2": "ñatvā sampākamattano;",
"ja503:31.3": "Asante nopaseveyya,",
"ja503:31.4": "sante seveyya paṇḍito;",
"ja503:31.5": "Asanto nirayaṃ nenti,",
"ja503:31.6": "santo pāpenti suggatin”ti.",
"ja503:32.1": "Sattigumbajātakaṃ sattamaṃ."
}