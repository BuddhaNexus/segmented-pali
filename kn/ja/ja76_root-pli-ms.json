{
"ja76:1.1": "Jātaka",
"ja76:2.1": "Ekakanipāta",
"ja76:3.1": "Varuṇavagga",
"ja76:4.1": "6. Asaṃkiyajātaka",
"ja76:5.1": "“Asaṅkiyomhi gāmamhi,",
"ja76:5.2": "araññe natthi me bhayaṃ;",
"ja76:5.3": "Ujuṃ maggaṃ samārūḷho,",
"ja76:5.4": "mettāya karuṇāya cā”ti.",
"ja76:6.1": "Asaṃkiyajātakaṃ chaṭṭhaṃ."
}