{
"ja106:1.1": "Jātaka",
"ja106:2.1": "Ekakanipāta",
"ja106:3.1": "Parosatavagga",
"ja106:4.1": "6. Udañcanījātaka",
"ja106:5.1": "“Sukhaṃ vata maṃ jīvantaṃ,",
"ja106:5.2": "Pacamānā udañcanī;",
"ja106:5.3": "Corī jāyappavādena,",
"ja106:5.4": "Telaṃ loṇañca yācatī”ti.",
"ja106:6.1": "Udañcanījātakaṃ chaṭṭhaṃ."
}