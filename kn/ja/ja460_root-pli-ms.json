{
"ja460:1.1": "Jātaka",
"ja460:2.1": "Ekādasakanipāta",
"ja460:3.1": "Mātuposakavagga",
"ja460:4.1": "6. Yudhañcayajātaka",
"ja460:5.1": "“Mittāmaccaparibyūḷhaṃ,",
"ja460:5.2": "ahaṃ vande rathesabhaṃ;",
"ja460:5.3": "Pabbajissāmahaṃ rāja,",
"ja460:5.4": "taṃ devo anumaññatu”.",
"ja460:6.1": "“Sace te ūnaṃ kāmehi,",
"ja460:6.2": "ahaṃ paripūrayāmi te;",
"ja460:6.3": "Yo taṃ hiṃsati vāremi,",
"ja460:6.4": "mā pabbaja yudhañcaya”.",
"ja460:7.1": "“Na matthi ūnaṃ kāmehi,",
"ja460:7.2": "hiṃsitā me na vijjati;",
"ja460:7.3": "Dīpañca kātumicchāmi,",
"ja460:7.4": "yaṃ jarā nābhikīrati”.",
"ja460:8.1": "“Putto vā pitaraṃ yāce,",
"ja460:8.2": "pitā vā puttamorasaṃ;",
"ja460:8.3": "Negamo taṃ yāce tāta,",
"ja460:8.4": "mā pabbaja yudhañcaya”.",
"ja460:9.1": "“Mā maṃ deva nivārehi,",
"ja460:9.2": "pabbajantaṃ rathesabha;",
"ja460:9.3": "Māhaṃ kāmehi sammatto,",
"ja460:9.4": "jarāya vasamanvagū”.",
"ja460:10.1": "“Ahaṃ taṃ tāta yācāmi,",
"ja460:10.2": "ahaṃ putta nivāraye;",
"ja460:10.3": "Ciraṃ taṃ daṭṭhumicchāmi,",
"ja460:10.4": "mā pabbaja yudhañcaya”.",
"ja460:11.1": "“Ussāvova tiṇaggamhi,",
"ja460:11.2": "sūriyuggamanaṃ pati;",
"ja460:11.3": "Evamāyu manussānaṃ,",
"ja460:11.4": "mā maṃ amma nivāraya”.",
"ja460:12.1": "“Taramāno imaṃ yānaṃ,",
"ja460:12.2": "āropetu rathesabha;",
"ja460:12.3": "Mā me mātā tarantassa,",
"ja460:12.4": "antarāyakarā ahu”.",
"ja460:13.1": "“Abhidhāvatha bhaddante,",
"ja460:13.2": "suññaṃ hessati rammakaṃ;",
"ja460:13.3": "Yudhañcayo anuññāto,",
"ja460:13.4": "sabbadattena rājinā.",
"ja460:14.1": "Yohu seṭṭho sahassassa,",
"ja460:14.2": "yuvā kañcanasannibho;",
"ja460:14.3": "Soyaṃ kumāro pabbajito,",
"ja460:14.4": "kāsāyavasano balī”.",
"ja460:15.1": "“Ubho kumārā pabbajitā,",
"ja460:15.2": "yudhañcayo yudhiṭṭhilo;",
"ja460:15.3": "Pahāya mātāpitaro,",
"ja460:15.4": "saṅgaṃ chetvāna maccuno”ti.",
"ja460:16.1": "Yudhañcayajātakaṃ chaṭṭhaṃ."
}