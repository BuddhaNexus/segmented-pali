{
"ja448:1.1": "Jātaka",
"ja448:2.1": "Dasakanipāta",
"ja448:3.1": "Catudvāravagga",
"ja448:4.1": "10. Kukkuṭajātaka",
"ja448:5.1": "“Nāsmase katapāpamhi,",
"ja448:5.2": "nāsmase alikavādine;",
"ja448:5.3": "Nāsmase attatthapaññamhi,",
"ja448:5.4": "atisantepi nāsmase.",
"ja448:6.1": "Bhavanti heke purisā,",
"ja448:6.2": "gopipāsikajātikā;",
"ja448:6.3": "Ghasanti maññe mittāni,",
"ja448:6.4": "vācāya na ca kammunā.",
"ja448:7.1": "Sukkhañjalipaggahitā,",
"ja448:7.2": "vācāya paliguṇṭhitā;",
"ja448:7.3": "Manussapheggū nāsīde,",
"ja448:7.4": "yasmiṃ natthi kataññutā.",
"ja448:8.1": "Na hi aññaññacittānaṃ,",
"ja448:8.2": "itthīnaṃ purisāna vā;",
"ja448:8.3": "Nānāvikatvā saṃsaggaṃ,",
"ja448:8.4": "tādisampi ca nāsmase.",
"ja448:9.1": "Anariyakammamokkantaṃ,",
"ja448:9.2": "athetaṃ sabbaghātinaṃ;",
"ja448:9.3": "Nisitaṃva paṭicchannaṃ,",
"ja448:9.4": "tādisampi ca nāsmase.",
"ja448:10.1": "Mittarūpenidhekacce,",
"ja448:10.2": "sākhalyena acetasā;",
"ja448:10.3": "Vividhehi upāyanti,",
"ja448:10.4": "tādisampi ca nāsmase.",
"ja448:11.1": "Āmisaṃ vā dhanaṃ vāpi,",
"ja448:11.2": "yattha passati tādiso;",
"ja448:11.3": "Dubbhiṃ karoti dummedho,",
"ja448:11.4": "tañca hantvāna gacchati”.",
"ja448:12.1": "“Mittarūpena bahavo,",
"ja448:12.2": "channā sevanti sattavo;",
"ja448:12.3": "Jahe kāpurise hete,",
"ja448:12.4": "kukkuṭo viya senakaṃ.",
"ja448:13.1": "Yo ca uppatitaṃ atthaṃ,",
"ja448:13.2": "na khippamanubujjhati;",
"ja448:13.3": "Amittavasamanveti,",
"ja448:13.4": "pacchā ca anutappati.",
"ja448:14.1": "Yo ca uppatitaṃ atthaṃ,",
"ja448:14.2": "khippameva nibodhati;",
"ja448:14.3": "Muccate sattusambādhā,",
"ja448:14.4": "kukkuṭo viya senakā.",
"ja448:15.1": "Taṃ tādisaṃ kūṭamivoḍḍitaṃ vane,",
"ja448:15.2": "Adhammikaṃ niccavidhaṃsakārinaṃ;",
"ja448:15.3": "Ārā vivajjeyya naro vicakkhaṇo,",
"ja448:15.4": "Senaṃ yathā kukkuṭo vaṃsakānane”ti.",
"ja448:16.1": "Kukkuṭajātakaṃ dasamaṃ."
}