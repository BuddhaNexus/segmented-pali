{
"ja376:1.1": "Jātaka",
"ja376:2.1": "Chakkanipāta",
"ja376:3.1": "Avāriyavagga",
"ja376:4.1": "1. Avāriyajātaka",
"ja376:5.1": "“Māsu kujjha bhūmipati,",
"ja376:5.2": "māsu kujjha rathesabha;",
"ja376:5.3": "Kuddhaṃ appaṭikujjhanto,",
"ja376:5.4": "rājā raṭṭhassa pūjito.",
"ja376:6.1": "Gāme vā yadi vāraññe,",
"ja376:6.2": "ninne vā yadi vā thale;",
"ja376:6.3": "Sabbattha anusāsāmi,",
"ja376:6.4": "māsu kujjha rathesabha”.",
"ja376:7.1": "“Avāriyapitā nāma,",
"ja376:7.2": "ahu gaṅgāya nāviko;",
"ja376:7.3": "Pubbe janaṃ tāretvāna,",
"ja376:7.4": "pacchā yācati vetanaṃ;",
"ja376:7.5": "Tenassa bhaṇḍanaṃ hoti,",
"ja376:7.6": "na ca bhogehi vaḍḍhati”.",
"ja376:8.1": "“Atiṇṇaṃyeva yācassu,",
"ja376:8.2": "apāraṃ tāta nāvika;",
"ja376:8.3": "Añño hi tiṇṇassa mano,",
"ja376:8.4": "añño hoti pāresino.",
"ja376:9.1": "Gāme vā yadi vāraññe,",
"ja376:9.2": "ninne vā yadi vā thale;",
"ja376:9.3": "Sabbattha anusāsāmi,",
"ja376:9.4": "māsu kujjhittha nāvika”.",
"ja376:10.1": "“Yāyevānusāsaniyā,",
"ja376:10.2": "rājā gāmavaraṃ adā;",
"ja376:10.3": "Tāyevānusāsaniyā,",
"ja376:10.4": "nāviko paharī mukhaṃ.",
"ja376:11.1": "Bhattaṃ bhinnaṃ hatā bhariyā,",
"ja376:11.2": "Gabbho ca patito chamā;",
"ja376:11.3": "Migova jātarūpena,",
"ja376:11.4": "Na tenatthaṃ abandhi sū”ti.",
"ja376:12.1": "Avāriyajātakaṃ paṭhamaṃ."
}