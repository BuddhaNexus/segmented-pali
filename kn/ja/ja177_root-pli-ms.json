{
"ja177:1.1": "Jātaka",
"ja177:2.1": "Dukanipāta",
"ja177:3.1": "Kalyāṇavagga",
"ja177:4.1": "7. Tindukajātaka",
"ja177:5.1": "“Dhanuhatthakalāpehi,",
"ja177:5.2": "nettiṃ savaradhāribhi;",
"ja177:5.3": "Samantā parikiṇṇamha,",
"ja177:5.4": "kathaṃ mokkho bhavissati”.",
"ja177:6.1": "“Appeva bahukiccānaṃ,",
"ja177:6.2": "attho jāyetha koci naṃ;",
"ja177:6.3": "Atthi rukkhassa acchinnaṃ,",
"ja177:6.4": "khajjathaññeva tindukan”ti.",
"ja177:7.1": "Tindukajātakaṃ sattamaṃ."
}