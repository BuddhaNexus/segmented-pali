{
"ja372:1.1": "Jātaka",
"ja372:2.1": "Pañcakanipāta",
"ja372:3.1": "Aḍḍhavagga",
"ja372:4.1": "2. Migapotakajātaka",
"ja372:5.1": "“Agārā paccupetassa,",
"ja372:5.2": "anagārassa te sato;",
"ja372:5.3": "Samaṇassa na taṃ sādhu,",
"ja372:5.4": "yaṃ petamanusocasi”.",
"ja372:6.1": "“Saṃvāsena have sakka,",
"ja372:6.2": "manussassa migassa vā;",
"ja372:6.3": "Hadaye jāyate pemaṃ,",
"ja372:6.4": "na taṃ sakkā asocituṃ”.",
"ja372:7.1": "“Mataṃ marissaṃ rodanti,",
"ja372:7.2": "ye rudanti lapanti ca;",
"ja372:7.3": "Tasmā tvaṃ isi mā rodi,",
"ja372:7.4": "roditaṃ moghamāhu santo.",
"ja372:8.1": "Roditena have brahme,",
"ja372:8.2": "mato peto samuṭṭhahe;",
"ja372:8.3": "Sabbe saṅgamma rodāma,",
"ja372:8.4": "aññamaññassa ñātake”.",
"ja372:9.1": "“Ādittaṃ vata maṃ santaṃ,",
"ja372:9.2": "ghatasittaṃva pāvakaṃ;",
"ja372:9.3": "Vārinā viya osiñcaṃ,",
"ja372:9.4": "sabbaṃ nibbāpaye daraṃ.",
"ja372:10.1": "Abbahi vata me sallaṃ,",
"ja372:10.2": "yamāsi hadayassitaṃ;",
"ja372:10.3": "Yo me sokaparetassa,",
"ja372:10.4": "puttasokaṃ apānudi.",
"ja372:11.1": "Sohaṃ abbūḷhasallosmi,",
"ja372:11.2": "vītasoko anāvilo;",
"ja372:11.3": "Na socāmi na rodāmi,",
"ja372:11.4": "tava sutvāna vāsavā”ti.",
"ja372:12.1": "Migapotakajātakaṃ dutiyaṃ."
}