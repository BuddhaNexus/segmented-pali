{
"ja176:1.1": "Jātaka",
"ja176:2.1": "Dukanipāta",
"ja176:3.1": "Kalyāṇavagga",
"ja176:4.1": "6. Kaḷāyamuṭṭhijātaka",
"ja176:5.1": "“Bālo vatāyaṃ dumasākhagocaro,",
"ja176:5.2": "Paññā janinda nayimassa vijjati;",
"ja176:5.3": "Kaḷāyamuṭṭhiṃ avakiriya kevalaṃ,",
"ja176:5.4": "Ekaṃ kaḷāyaṃ patitaṃ gavesati”.",
"ja176:6.1": "“Evameva mayaṃ rāja,",
"ja176:6.2": "ye caññe atilobhino;",
"ja176:6.3": "Appena bahuṃ jiyyāma,",
"ja176:6.4": "kaḷāyeneva vānaro”ti.",
"ja176:7.1": "Kaḷāyamuṭṭhijātakaṃ chaṭṭhaṃ."
}