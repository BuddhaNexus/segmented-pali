{
"ja245:1.1": "Jātaka",
"ja245:2.1": "Dukanipāta",
"ja245:3.1": "Siṅgālavagga",
"ja245:4.1": "5. Mūlapariyāyajātaka",
"ja245:5.1": "“Kālo ghasati bhūtāni,",
"ja245:5.2": "sabbāneva sahattanā;",
"ja245:5.3": "Yo ca kālaghaso bhūto,",
"ja245:5.4": "sa bhūtapacaniṃ paci”.",
"ja245:6.1": "“Bahūni narasīsāni,",
"ja245:6.2": "lomasāni brahāni ca;",
"ja245:6.3": "Gīvāsu paṭimukkāni,",
"ja245:6.4": "kocidevettha kaṇṇavā”ti.",
"ja245:7.1": "Mūlapariyāyajātakaṃ pañcamaṃ."
}