{
"ja267:1.1": "Jātaka",
"ja267:2.1": "Tikanipāta",
"ja267:3.1": "Padumavagga",
"ja267:4.1": "7. Kakkaṭakajātaka",
"ja267:5.1": "“Siṅgīmigo āyatacakkhunetto,",
"ja267:5.2": "Aṭṭhittaco vārisayo alomo;",
"ja267:5.3": "Tenābhibhūto kapaṇaṃ rudāmi,",
"ja267:5.4": "Mā heva maṃ pāṇasamaṃ jaheyya”.",
"ja267:6.1": "“Ayya na taṃ jahissāmi,",
"ja267:6.2": "kuñjaraṃ saṭṭhihāyanaṃ;",
"ja267:6.3": "Pathabyā cāturantāya,",
"ja267:6.4": "suppiyo hosi me tuvaṃ”.",
"ja267:7.1": "“Ye kuḷīrā samuddasmiṃ,",
"ja267:7.2": "gaṅgāya yamunāya ca;",
"ja267:7.3": "Tesaṃ tvaṃ vārijo seṭṭho,",
"ja267:7.4": "muñca rodantiyā patin”ti.",
"ja267:8.1": "Kakkaṭakajātakaṃ sattamaṃ."
}