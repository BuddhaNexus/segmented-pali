{
"ja332:1.1": "Jātaka",
"ja332:2.1": "Catukkanipāta",
"ja332:3.1": "Kokilavagga",
"ja332:4.1": "2. Rathalaṭṭhijātaka",
"ja332:5.1": "“Api hantvā hato brūti,",
"ja332:5.2": "jetvā jitoti bhāsati;",
"ja332:5.3": "Pubbamakkhāyino rāja,",
"ja332:5.4": "aññadatthu na saddahe.",
"ja332:6.1": "Tasmā paṇḍitajātiyo,",
"ja332:6.2": "suṇeyya itarassapi;",
"ja332:6.3": "Ubhinnaṃ vacanaṃ sutvā,",
"ja332:6.4": "yathā dhammo tathā kare.",
"ja332:7.1": "Alaso gihī kāmabhogī na sādhu,",
"ja332:7.2": "Asaññato pabbajito na sādhu;",
"ja332:7.3": "Rājā na sādhu anisammakārī,",
"ja332:7.4": "Yo paṇḍito kodhano taṃ na sādhu.",
"ja332:8.1": "Nisamma khattiyo kayirā,",
"ja332:8.2": "nānisamma disampati;",
"ja332:8.3": "Nisammakārino rāja,",
"ja332:8.4": "yaso kitti ca vaḍḍhatī”ti.",
"ja332:9.1": "Rathalaṭṭhijātakaṃ dutiyaṃ."
}