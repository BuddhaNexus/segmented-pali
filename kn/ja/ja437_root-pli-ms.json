{
"ja437:1.1": "Jātaka",
"ja437:2.1": "Navakanipāta",
"ja437:3.1": "Gijjhavagga",
"ja437:4.1": "11. Pūtimaṃsajātaka",
"ja437:5.1": "“Na kho me ruccati āḷi,",
"ja437:5.2": "pūtimaṃsassa pekkhanā;",
"ja437:5.3": "Etādisā sakhārasmā,",
"ja437:5.4": "ārakā parivajjaye”.",
"ja437:6.1": "“Ummattikā ayaṃ veṇī,",
"ja437:6.2": "vaṇṇeti patino sakhiṃ;",
"ja437:6.3": "Pajjhāyi paṭigacchantiṃ,",
"ja437:6.4": "āgataṃ meṇḍamātaraṃ”.",
"ja437:7.1": "“Tvaṃ khosi samma ummatto,",
"ja437:7.2": "dummedho avicakkhaṇo;",
"ja437:7.3": "Yo tvaṃ matālayaṃ katvā,",
"ja437:7.4": "akālena vipekkhasi”.",
"ja437:8.1": "“Na akāle vipekkheyya,",
"ja437:8.2": "kāle pekkheyya paṇḍito;",
"ja437:8.3": "Pūtimaṃsova pajjhāyi,",
"ja437:8.4": "yo akāle vipekkhati”.",
"ja437:9.1": "“Piyaṃ kho āḷi me hotu,",
"ja437:9.2": "puṇṇapattaṃ dadāhi me;",
"ja437:9.3": "Pati sañjīvito mayhaṃ,",
"ja437:9.4": "eyyāsi piyapucchikā”.",
"ja437:10.1": "“Piyaṃ kho āḷi te hotu,",
"ja437:10.2": "puṇṇapattaṃ dadāmi te;",
"ja437:10.3": "Mahatā parivārena,",
"ja437:10.4": "essaṃ kayirāhi bhojanaṃ”.",
"ja437:11.1": "“Kīdiso tuyhaṃ parivāro,",
"ja437:11.2": "yesaṃ kāhāmi bhojanaṃ;",
"ja437:11.3": "Kiṃnāmakā ca te sabbe,",
"ja437:11.4": "taṃ me akkhāhi pucchitā”.",
"ja437:12.1": "“Māliyo caturakkho ca,",
"ja437:12.2": "piṅgiyo atha jambuko;",
"ja437:12.3": "Ediso mayhaṃ parivāro,",
"ja437:12.4": "tesaṃ kayirāhi bhojanaṃ”.",
"ja437:13.1": "“Nikkhantāya agārasmā,",
"ja437:13.2": "bhaṇḍakampi vinassati;",
"ja437:13.3": "Ārogyaṃ āḷino vajjaṃ,",
"ja437:13.4": "idheva vasa māgamā”ti.",
"ja437:14.1": "Pūtimaṃsajātakaṃ ekādasamaṃ."
}