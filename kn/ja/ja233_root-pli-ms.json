{
"ja233:1.1": "Jātaka",
"ja233:2.1": "Dukanipāta",
"ja233:3.1": "Upāhanavagga",
"ja233:4.1": "3. Vikaṇṇajātaka",
"ja233:5.1": "“Kāmaṃ yahiṃ icchasi tena gaccha,",
"ja233:5.2": "Viddhosi mammamhi vikaṇṇakena;",
"ja233:5.3": "Hatosi bhattena suvāditena,",
"ja233:5.4": "Lolo ca macche anubandhamāno”.",
"ja233:6.1": "“Evampi lokāmisaṃ opatanto,",
"ja233:6.2": "Vihaññatī cittavasānuvattī;",
"ja233:6.3": "So haññatī ñātisakhāna majjhe,",
"ja233:6.4": "Macchānugo soriva suṃsumāro”ti.",
"ja233:7.1": "Vikaṇṇajātakaṃ tatiyaṃ."
}