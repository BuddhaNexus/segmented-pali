{
"ja270:1.1": "Jātaka",
"ja270:2.1": "Tikanipāta",
"ja270:3.1": "Padumavagga",
"ja270:4.1": "10. Ulūkajātaka",
"ja270:5.1": "“Sabbehi kira ñātīhi,",
"ja270:5.2": "kosiyo issaro kato;",
"ja270:5.3": "Sace ñātīhi anuññāto,",
"ja270:5.4": "bhaṇeyyāhaṃ ekavācikaṃ”.",
"ja270:6.1": "“Bhaṇa samma anuññāto,",
"ja270:6.2": "atthaṃ dhammañca kevalaṃ;",
"ja270:6.3": "Santi hi daharā pakkhī,",
"ja270:6.4": "paññavanto jutindharā”.",
"ja270:7.1": "“Na me ruccati bhaddaṃ vo,",
"ja270:7.2": "ulūkassābhisecanaṃ;",
"ja270:7.3": "Akkuddhassa mukhaṃ passa,",
"ja270:7.4": "kathaṃ kuddho karissatī”ti.",
"ja270:8.1": "Ulūkajātakaṃ dasamaṃ.",
"ja270:8.2": "Padumavaggo dutiyo.",
"ja270:9.1": "Tassuddānaṃ",
"ja270:10.1": "Padumuttama nāgasirivhayano,",
"ja270:10.2": "Sa-mahaṇṇava yūpa khurappavaro;",
"ja270:10.3": "Atha bhaddalī kuñjara rukkha puna,",
"ja270:10.4": "Kharavāca ulūkavarena dasāti."
}