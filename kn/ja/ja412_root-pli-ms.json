{
"ja412:1.1": "Jātaka",
"ja412:2.1": "Sattakanipāta",
"ja412:3.1": "Gandhāravagga",
"ja412:4.1": "7. Koṭasimbalijātaka",
"ja412:5.1": "“Ahaṃ dasasataṃbyāmaṃ,",
"ja412:5.2": "uragamādāya āgato;",
"ja412:5.3": "Tañca mañca mahākāyaṃ,",
"ja412:5.4": "dhārayaṃ nappavedhasi.",
"ja412:6.1": "Athimaṃ khuddakaṃ pakkhiṃ,",
"ja412:6.2": "Appamaṃsataraṃ mayā;",
"ja412:6.3": "Dhārayaṃ byathasi bhītā,",
"ja412:6.4": "Kamatthaṃ koṭasimbali”.",
"ja412:7.1": "“Maṃsabhakkho tuvaṃ rāja,",
"ja412:7.2": "phalabhakkho ayaṃ dijo;",
"ja412:7.3": "Ayaṃ nigrodhabījāni,",
"ja412:7.4": "pilakkhudumbarāni ca;",
"ja412:7.5": "Assatthāni ca bhakkhitvā,",
"ja412:7.6": "khandhe me ohadissati.",
"ja412:8.1": "Te rukkhā saṃvirūhanti,",
"ja412:8.2": "mama passe nivātajā;",
"ja412:8.3": "Te maṃ pariyonandhissanti,",
"ja412:8.4": "arukkhaṃ maṃ karissare.",
"ja412:9.1": "Santi aññepi rukkhā se,",
"ja412:9.2": "mūlino khandhino dumā;",
"ja412:9.3": "Iminā sakuṇajātena,",
"ja412:9.4": "bījamāharitā hatā.",
"ja412:10.1": "Ajjhārūhābhivaḍḍhanti,",
"ja412:10.2": "brahantampi vanappatiṃ;",
"ja412:10.3": "Tasmā rāja pavedhāmi,",
"ja412:10.4": "sampassaṃnāgataṃ bhayaṃ”.",
"ja412:11.1": "“Saṅkeyya saṅkitabbāni,",
"ja412:11.2": "rakkheyyānāgataṃ bhayaṃ;",
"ja412:11.3": "Anāgatabhayā dhīro,",
"ja412:11.4": "ubho loke avekkhatī”ti.",
"ja412:12.1": "Koṭasimbalijātakaṃ sattamaṃ."
}