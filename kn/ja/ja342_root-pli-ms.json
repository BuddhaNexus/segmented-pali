{
"ja342:1.1": "Jātaka",
"ja342:2.1": "Catukkanipāta",
"ja342:3.1": "Cūḷakuṇālavagga",
"ja342:4.1": "2. Vānarajātaka",
"ja342:5.1": "“Asakkhiṃ vata attānaṃ,",
"ja342:5.2": "uddhātuṃ udakā thalaṃ;",
"ja342:5.3": "Na dānāhaṃ puna tuyhaṃ,",
"ja342:5.4": "vasaṃ gacchāmi vārija.",
"ja342:6.1": "Alametehi ambehi,",
"ja342:6.2": "jambūhi panasehi ca;",
"ja342:6.3": "Yāni pāraṃ samuddassa,",
"ja342:6.4": "varaṃ mayhaṃ udumbaro.",
"ja342:7.1": "Yo ca uppatitaṃ atthaṃ,",
"ja342:7.2": "na khippamanubujjhati;",
"ja342:7.3": "Amittavasamanveti,",
"ja342:7.4": "pacchā ca anutappati.",
"ja342:8.1": "Yo ca uppatitaṃ atthaṃ,",
"ja342:8.2": "khippameva nibodhati;",
"ja342:8.3": "Muccate sattusambādhā,",
"ja342:8.4": "na ca pacchānutappatī”ti.",
"ja342:9.1": "Vānarajātakaṃ dutiyaṃ."
}