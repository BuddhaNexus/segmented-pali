{
"ja120:1.1": "Jātaka",
"ja120:2.1": "Ekakanipāta",
"ja120:3.1": "Haṃcivagga",
"ja120:4.1": "10. Bandhanamokkhajātaka",
"ja120:5.1": "“Abaddhā tattha bajjhanti,",
"ja120:5.2": "yattha bālā pabhāsare;",
"ja120:5.3": "Baddhāpi tattha muccanti,",
"ja120:5.4": "yattha dhīrā pabhāsare”ti.",
"ja120:6.1": "Bandhanamokkhajātakaṃ dasamaṃ.",
"ja120:6.2": "Haṃcivaggo dvādasamo.",
"ja120:7.1": "Tassuddānaṃ",
"ja120:8.1": "Atha gadrabha sattuva kaṃsasataṃ,",
"ja120:8.2": "Bahucinti sāsikāyātikara;",
"ja120:8.3": "Ativela visesamanācariyova,",
"ja120:8.4": "Dhīrāpabhāsaratena dasāti."
}