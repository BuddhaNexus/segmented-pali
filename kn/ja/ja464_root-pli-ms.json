{
"ja464:1.1": "Jātaka",
"ja464:2.1": "Dvādasakanipāta",
"ja464:3.1": "Cūḷakuṇālavagga",
"ja464:4.1": "1. Cūḷakuṇālajātaka",
"ja464:5.1": "“Luddhānaṃ lahucittānaṃ,",
"ja464:5.2": "akataññūna dubbhinaṃ;",
"ja464:5.3": "Nādevasatto puriso,",
"ja464:5.4": "thīnaṃ saddhātumarahati.",
"ja464:6.1": "Na tā pajānanti kataṃ na kiccaṃ,",
"ja464:6.2": "Na mātaraṃ pitaraṃ bhātaraṃ vā;",
"ja464:6.3": "Anariyā samatikkantadhammā,",
"ja464:6.4": "Sasseva cittassa vasaṃ vajanti.",
"ja464:7.1": "Cirānuvutthampi piyaṃ manāpaṃ,",
"ja464:7.2": "Anukampakaṃ pāṇasamampi bhattuṃ;",
"ja464:7.3": "Āvāsu kiccesu ca naṃ jahanti,",
"ja464:7.4": "Tasmāhamitthīnaṃ na vissasāmi.",
"ja464:8.1": "Thīnañhi cittaṃ yathā vānarassa,",
"ja464:8.2": "Kannappakannaṃ yathā rukkhachāyā;",
"ja464:8.3": "Calācalaṃ hadayamitthiyānaṃ,",
"ja464:8.4": "Cakkassa nemi viya parivattati.",
"ja464:9.1": "Yadā tā passanti samekkhamānā,",
"ja464:9.2": "Ādeyyarūpaṃ purisassa vittaṃ;",
"ja464:9.3": "Saṇhāhi vācāhi nayanti menaṃ,",
"ja464:9.4": "Kambojakā jalajeneva assaṃ.",
"ja464:10.1": "Yadā na passanti samekkhamānā,",
"ja464:10.2": "Ādeyyarūpaṃ purisassa vittaṃ;",
"ja464:10.3": "Samantato naṃ parivajjayanti,",
"ja464:10.4": "Tiṇṇo nadīpāragatova kullaṃ.",
"ja464:11.1": "Silesūpamā sikhiriva sabbabhakkhā,",
"ja464:11.2": "Tikkhamāyā nadīriva sīghasotā;",
"ja464:11.3": "Sevanti hetā piyamappiyañca,",
"ja464:11.4": "Nāvā yathā orakūlaṃ parañca.",
"ja464:12.1": "Na tā ekassa na dvinnaṃ,",
"ja464:12.2": "āpaṇova pasārito;",
"ja464:12.3": "Yo tā mayhanti maññeyya,",
"ja464:12.4": "vātaṃ jālena bādhaye.",
"ja464:13.1": "Yathā nadī ca pantho ca,",
"ja464:13.2": "pānāgāraṃ sabhā papā;",
"ja464:13.3": "Evaṃ lokitthiyo nāma,",
"ja464:13.4": "velā tāsaṃ na vijjati.",
"ja464:14.1": "Ghatāsanasamā etā,",
"ja464:14.2": "kaṇhasappasirūpamā;",
"ja464:14.3": "Gāvo bahitiṇasseva,",
"ja464:14.4": "omasanti varaṃ varaṃ.",
"ja464:15.1": "Ghatāsanaṃ kuñjaraṃ kaṇhasappaṃ,",
"ja464:15.2": "Muddhābhisittaṃ pamadā ca sabbā;",
"ja464:15.3": "Ete naro niccayato bhajetha,",
"ja464:15.4": "Tesaṃ have dubbidu sabbabhāvo.",
"ja464:16.1": "Naccantavaṇṇā na bahūnaṃ kantā,",
"ja464:16.2": "Na dakkhiṇā pamadā sevitabbā;",
"ja464:16.3": "Na parassa bhariyā na dhanassa hetu,",
"ja464:16.4": "Etitthiyo pañca na sevitabbā”ti.",
"ja464:17.1": "Cūḷakuṇālajātakaṃ paṭhamaṃ."
}