{
"ja88:1.1": "Jātaka",
"ja88:2.1": "Ekakanipāta",
"ja88:3.1": "Apāyimhavagga",
"ja88:4.1": "8. Sārambhajātaka",
"ja88:5.1": "“Kalyāṇimeva muñceyya,",
"ja88:5.2": "na hi muñceyya pāpikaṃ;",
"ja88:5.3": "Mokkho kalyāṇiyā sādhu,",
"ja88:5.4": "mutvā tappati pāpikan”ti.",
"ja88:6.1": "Sārambhajātakaṃ aṭṭhamaṃ."
}