{
"ja394:1.1": "Jātaka",
"ja394:2.1": "Chakkanipāta",
"ja394:3.1": "Kharaputtavagga",
"ja394:4.1": "9. Vaṭṭakajātaka",
"ja394:5.1": "“Paṇītaṃ bhuñjase bhattaṃ,",
"ja394:5.2": "sappitelañca mātula;",
"ja394:5.3": "Atha kena nu vaṇṇena,",
"ja394:5.4": "kiso tvamasi vāyasa”.",
"ja394:6.1": "“Amittamajjhe vasato,",
"ja394:6.2": "tesu āmisamesato;",
"ja394:6.3": "Niccaṃ ubbiggahadayassa,",
"ja394:6.4": "kuto kākassa daḷhiyaṃ.",
"ja394:7.1": "Niccaṃ ubbegino kākā,",
"ja394:7.2": "dhaṅkā pāpena kammunā;",
"ja394:7.3": "Laddho piṇḍo na pīṇeti,",
"ja394:7.4": "kiso tenasmi vaṭṭaka.",
"ja394:8.1": "Lūkhāni tiṇabījāni,",
"ja394:8.2": "appasnehāni bhuñjasi;",
"ja394:8.3": "Atha kena nu vaṇṇena,",
"ja394:8.4": "thūlo tvamasi vaṭṭaka”.",
"ja394:9.1": "“Appicchā appacintāya,",
"ja394:9.2": "adūragamanena ca;",
"ja394:9.3": "Laddhāladdhena yāpento,",
"ja394:9.4": "thūlo tenasmi vāyasa.",
"ja394:10.1": "Appicchassa hi posassa,",
"ja394:10.2": "appacintasukhassa ca;",
"ja394:10.3": "Susaṅgahitamānassa,",
"ja394:10.4": "vuttī susamudānayā”ti.",
"ja394:11.1": "Vaṭṭakajātakaṃ navamaṃ."
}