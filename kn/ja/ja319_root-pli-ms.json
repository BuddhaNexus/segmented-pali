{
"ja319:1.1": "Jātaka",
"ja319:2.1": "Catukkanipāta",
"ja319:3.1": "Pucimandavagga",
"ja319:4.1": "9. Tittirajātaka",
"ja319:5.1": "“Susukhaṃ vata jīvāmi,",
"ja319:5.2": "labhāmi ceva bhuñjituṃ;",
"ja319:5.3": "Paripanthe ca tiṭṭhāmi,",
"ja319:5.4": "kā nu bhante gatī mama”.",
"ja319:6.1": "“Mano ce te nappaṇamati,",
"ja319:6.2": "pakkhi pāpassa kammuno;",
"ja319:6.3": "Abyāvaṭassa bhadrassa,",
"ja319:6.4": "na pāpamupalimpati”.",
"ja319:7.1": "“Ñātako no nisinnoti,",
"ja319:7.2": "bahu āgacchate jano;",
"ja319:7.3": "Paṭicca kammaṃ phusati,",
"ja319:7.4": "tasmiṃ me saṅkate mano”.",
"ja319:8.1": "“Na paṭicca kammaṃ phusati,",
"ja319:8.2": "mano ce nappadussati;",
"ja319:8.3": "Appossukkassa bhadrassa,",
"ja319:8.4": "na pāpamupalimpatī”ti.",
"ja319:9.1": "Tittirajātakaṃ navamaṃ."
}