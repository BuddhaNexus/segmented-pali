{
"ja169:1.1": "Jātaka",
"ja169:2.1": "Dukanipāta",
"ja169:3.1": "Santhavavagga",
"ja169:4.1": "9. Arakajātaka",
"ja169:5.1": "“Yo ve mettena cittena,",
"ja169:5.2": "sabbalokānukampati;",
"ja169:5.3": "Uddhaṃ adho ca tiriyaṃ,",
"ja169:5.4": "appamāṇena sabbaso.",
"ja169:6.1": "Appamāṇaṃ hitaṃ cittaṃ,",
"ja169:6.2": "paripuṇṇaṃ subhāvitaṃ;",
"ja169:6.3": "Yaṃ pamāṇakataṃ kammaṃ,",
"ja169:6.4": "na taṃ tatrāvasissatī”ti.",
"ja169:7.1": "Arakajātakaṃ navamaṃ."
}