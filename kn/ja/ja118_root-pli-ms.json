{
"ja118:1.1": "Jātaka",
"ja118:2.1": "Ekakanipāta",
"ja118:3.1": "Haṃcivagga",
"ja118:4.1": "8. Vaṭṭakajātaka",
"ja118:5.1": "“Nācintayanto puriso,",
"ja118:5.2": "visesamadhigacchati;",
"ja118:5.3": "Cintitassa phalaṃ passa,",
"ja118:5.4": "muttosmi vadhabandhanā”ti.",
"ja118:6.1": "Vaṭṭakajātakaṃ aṭṭhamaṃ."
}