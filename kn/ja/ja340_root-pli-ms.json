{
"ja340:1.1": "Jātaka",
"ja340:2.1": "Catukkanipāta",
"ja340:3.1": "Kokilavagga",
"ja340:4.1": "10. Visayhajātaka",
"ja340:5.1": "“Adāsi dānāni pure visayha,",
"ja340:5.2": "Dadato ca te khayadhammo ahosi;",
"ja340:5.3": "Ito parañce na dadeyya dānaṃ,",
"ja340:5.4": "Tiṭṭheyyuṃ te saṃyamantassa bhogā”.",
"ja340:6.1": "“Anariyamariyena sahassanetta,",
"ja340:6.2": "Suduggatenāpi akiccamāhu;",
"ja340:6.3": "Mā vo dhanaṃ taṃ ahu devarāja,",
"ja340:6.4": "Yaṃ bhogahetu vijahemu saddhaṃ.",
"ja340:7.1": "Yena eko ratho yāti,",
"ja340:7.2": "yāti tenāparo ratho;",
"ja340:7.3": "Porāṇaṃ nihitaṃ vattaṃ,",
"ja340:7.4": "vattataññeva vāsava.",
"ja340:8.1": "Yadi hessati dassāma,",
"ja340:8.2": "asante kiṃ dadāmase;",
"ja340:8.3": "Evaṃbhūtāpi dassāma,",
"ja340:8.4": "mā dānaṃ pamadamhase”ti.",
"ja340:9.1": "Visayhajātakaṃ dasamaṃ.",
"ja340:9.2": "Kokilavaggo catuttho.",
"ja340:10.1": "Tassuddānaṃ",
"ja340:11.1": "Ativelapabhāsati jītavaro,",
"ja340:11.2": "Vanamajjha rathesabha jimhagamo;",
"ja340:11.3": "Atha jambu tiṇāsanapīṭhavaraṃ,",
"ja340:11.4": "Atha taṇḍula mora visayha dasāti."
}