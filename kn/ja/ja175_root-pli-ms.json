{
"ja175:1.1": "Jātaka",
"ja175:2.1": "Dukanipāta",
"ja175:3.1": "Kalyāṇavagga",
"ja175:4.1": "5. Ādiccupaṭṭhānajātaka",
"ja175:5.1": "“Sabbesu kira bhūtesu,",
"ja175:5.2": "santi sīlasamāhitā;",
"ja175:5.3": "Passa sākhāmigaṃ jammaṃ,",
"ja175:5.4": "ādiccamupatiṭṭhati”.",
"ja175:6.1": "“Nāssa sīlaṃ vijānātha,",
"ja175:6.2": "anaññāya pasaṃsatha;",
"ja175:6.3": "Aggihuttañca uhannaṃ,",
"ja175:6.4": "dve ca bhinnā kamaṇḍalū”ti.",
"ja175:7.1": "Ādiccupaṭṭhānajātakaṃ pañcamaṃ."
}