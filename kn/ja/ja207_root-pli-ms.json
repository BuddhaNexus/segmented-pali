{
"ja207:1.1": "Jātaka",
"ja207:2.1": "Dukanipāta",
"ja207:3.1": "Nataṃdaḷhavagga",
"ja207:4.1": "7. Assakajātaka",
"ja207:5.1": "“Ayamassakarājena,",
"ja207:5.2": "deso vicarito mayā;",
"ja207:5.3": "Anukāmayakāmena,",
"ja207:5.4": "piyena patinā saha.",
"ja207:6.1": "Navena sukhadukkhena,",
"ja207:6.2": "porāṇaṃ apidhīyati;",
"ja207:6.3": "Tasmā assakaraññāva,",
"ja207:6.4": "kīṭo piyataro mamā”ti.",
"ja207:7.1": "Assakajātakaṃ sattamaṃ."
}