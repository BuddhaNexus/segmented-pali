{
"ja520:1.1": "Jātaka",
"ja520:2.1": "Tiṃsanipāta",
"ja520:3.1": "Kiṃchandavagga",
"ja520:4.1": "10. Gandhatindukajātaka",
"ja520:5.1": "“Appamādo amataṃ padaṃ,",
"ja520:5.2": "Pamādo maccuno padaṃ;",
"ja520:5.3": "Appamattā na mīyanti,",
"ja520:5.4": "Ye pamattā yathā matā.",
"ja520:6.1": "Madā pamādo jāyetha,",
"ja520:6.2": "pamādā jāyate khayo;",
"ja520:6.3": "Khayā padosā jāyanti,",
"ja520:6.4": "mā pamādo bharatūsabha.",
"ja520:7.1": "Bahū hi khattiyā jīnā,",
"ja520:7.2": "atthaṃ raṭṭhaṃ pamādino;",
"ja520:7.3": "Athopi gāmino gāmā,",
"ja520:7.4": "anagārā agārino.",
"ja520:8.1": "Khattiyassa pamattassa,",
"ja520:8.2": "raṭṭhasmiṃ raṭṭhavaḍḍhana;",
"ja520:8.3": "Sabbe bhogā vinassanti,",
"ja520:8.4": "rañño taṃ vuccate aghaṃ.",
"ja520:9.1": "Nesa dhammo mahārāja,",
"ja520:9.2": "ativelaṃ pamajjasi;",
"ja520:9.3": "Iddhaṃ phītaṃ janapadaṃ,",
"ja520:9.4": "corā viddhaṃsayanti naṃ.",
"ja520:10.1": "Na te puttā bhavissanti,",
"ja520:10.2": "na hiraññaṃ na dhāniyaṃ;",
"ja520:10.3": "Raṭṭhe viluppamānamhi,",
"ja520:10.4": "sabbabhogehi jiyyasi.",
"ja520:11.1": "Sabbabhogā parijiṇṇaṃ,",
"ja520:11.2": "rājānaṃ vāpi khattiyaṃ;",
"ja520:11.3": "Ñātimittā suhajjā ca,",
"ja520:11.4": "na taṃ maññanti māniyaṃ.",
"ja520:12.1": "Hatthārohā anikaṭṭhā,",
"ja520:12.2": "rathikā pattikārakā;",
"ja520:12.3": "Tamevamupajīvantā,",
"ja520:12.4": "na taṃ maññanti māniyaṃ.",
"ja520:13.1": "Asaṃvihitakammantaṃ,",
"ja520:13.2": "bālaṃ dummantimantinaṃ;",
"ja520:13.3": "Sirī jahati dummedhaṃ,",
"ja520:13.4": "jiṇṇaṃva urago tacaṃ.",
"ja520:14.1": "Susaṃvihitakammantaṃ,",
"ja520:14.2": "kāluṭṭhāyiṃ atanditaṃ;",
"ja520:14.3": "Sabbe bhogābhivaḍḍhanti,",
"ja520:14.4": "gāvo sausabhāmiva.",
"ja520:15.1": "Upassutiṃ mahārāja,",
"ja520:15.2": "raṭṭhe janapade cara;",
"ja520:15.3": "Tattha disvā ca sutvā ca,",
"ja520:15.4": "tato taṃ paṭipajjasi”.",
"ja520:16.1": "“Evaṃ vedetu pañcālo,",
"ja520:16.2": "saṅgāme saramappito;",
"ja520:16.3": "Yathāhamajja vedemi,",
"ja520:16.4": "kaṇṭakena samappito”.",
"ja520:17.1": "“Jiṇṇo dubbalacakkhūsi,",
"ja520:17.2": "na rūpaṃ sādhu passasi;",
"ja520:17.3": "Kiṃ tattha brahmadattassa,",
"ja520:17.4": "yaṃ taṃ maggeyya kaṇṭako”.",
"ja520:18.1": "“Bahvettha brahmadattassa,",
"ja520:18.2": "Sohaṃ maggasmi brāhmaṇa;",
"ja520:18.3": "Arakkhitā jānapadā,",
"ja520:18.4": "Adhammabalinā hatā.",
"ja520:19.1": "Rattiñhi corā khādanti,",
"ja520:19.2": "divā khādanti tuṇḍiyā;",
"ja520:19.3": "Raṭṭhasmiṃ kūṭarājassa,",
"ja520:19.4": "bahu adhammiko jano.",
"ja520:20.1": "Etādise bhaye jāte,",
"ja520:20.2": "bhayaṭṭā tāta māṇavā;",
"ja520:20.3": "Nillenakāni kubbanti,",
"ja520:20.4": "vane āhatva kaṇṭakaṃ”.",
"ja520:21.1": "“Kadāssu nāmayaṃ rājā,",
"ja520:21.2": "brahmadatto marissati;",
"ja520:21.3": "Yassa raṭṭhamhi jiyyanti,",
"ja520:21.4": "appatikā kumārikā”.",
"ja520:22.1": "“Dubbhāsitañhi te jammi,",
"ja520:22.2": "anatthapadakovide;",
"ja520:22.3": "Kuhiṃ rājā kumārīnaṃ,",
"ja520:22.4": "bhattāraṃ pariyesati”.",
"ja520:23.1": "“Na me dubbhāsitaṃ brahme,",
"ja520:23.2": "kovidatthapadā ahaṃ;",
"ja520:23.3": "Arakkhitā jānapadā,",
"ja520:23.4": "adhammabalinā hatā.",
"ja520:24.1": "Rattiñhi corā khādanti,",
"ja520:24.2": "divā khādanti tuṇḍiyā;",
"ja520:24.3": "Raṭṭhasmiṃ kūṭarājassa,",
"ja520:24.4": "bahu adhammiko jano;",
"ja520:24.5": "Dujjīve dubbhare dāre,",
"ja520:24.6": "kuto bhattā kumāriyo”.",
"ja520:25.1": "“Evaṃ sayatu pañcālo,",
"ja520:25.2": "saṅgāme sattiyā hato;",
"ja520:25.3": "Yathāyaṃ kapaṇo seti,",
"ja520:25.4": "hato phālena sāliyo”.",
"ja520:26.1": "“Adhammena tuvaṃ jamma,",
"ja520:26.2": "brahmadattassa kujjhasi;",
"ja520:26.3": "Yo tvaṃ sapasi rājānaṃ,",
"ja520:26.4": "aparajjhitvāna attano”.",
"ja520:27.1": "“Dhammena brahmadattassa,",
"ja520:27.2": "ahaṃ kujjhāmi brāhmaṇa;",
"ja520:27.3": "Arakkhitā jānapadā,",
"ja520:27.4": "adhammabalinā hatā.",
"ja520:28.1": "Rattiñhi corā khādanti,",
"ja520:28.2": "divā khādanti tuṇḍiyā;",
"ja520:28.3": "Raṭṭhasmiṃ kūṭarājassa,",
"ja520:28.4": "bahu adhammiko jano.",
"ja520:29.1": "Sā nūna puna re pakkā,",
"ja520:29.2": "vikāle bhattamāhari;",
"ja520:29.3": "Bhattahāriṃ apekkhanto,",
"ja520:29.4": "hato phālena sāliyo”.",
"ja520:30.1": "“Evaṃ haññatu pañcālo,",
"ja520:30.2": "saṅgāme asinā hato;",
"ja520:30.3": "Yathāhamajja pahato,",
"ja520:30.4": "khīrañca me pavaṭṭitaṃ”.",
"ja520:31.1": "“Yaṃ pasu khīraṃ chaḍḍeti,",
"ja520:31.2": "pasupālaṃ vihiṃsati;",
"ja520:31.3": "Kiṃ tattha brahmadattassa,",
"ja520:31.4": "yaṃ no garahate bhavaṃ”.",
"ja520:32.1": "“Gārayho brahme pañcālo,",
"ja520:32.2": "brahmadattassa rājino;",
"ja520:32.3": "Arakkhitā jānapadā,",
"ja520:32.4": "adhammabalinā hatā.",
"ja520:33.1": "Rattiñhi corā khādanti,",
"ja520:33.2": "divā khādanti tuṇḍiyā;",
"ja520:33.3": "Raṭṭhasmiṃ kūṭarājassa,",
"ja520:33.4": "bahu adhammiko jano.",
"ja520:34.1": "Caṇḍā aṭanakā gāvī,",
"ja520:34.2": "yaṃ pure na duhāmase;",
"ja520:34.3": "Taṃ dāni ajja dohāma,",
"ja520:34.4": "khīrakāmehupaddutā”.",
"ja520:35.1": "“Evaṃ kandatu pañcālo,",
"ja520:35.2": "viputto vippasukkhatu;",
"ja520:35.3": "Yathāyaṃ kapaṇā gāvī,",
"ja520:35.4": "viputtā paridhāvati”.",
"ja520:36.1": "“Yaṃ pasu pasupālassa,",
"ja520:36.2": "sambhameyya raveyya vā;",
"ja520:36.3": "Ko nīdha aparādhatthi,",
"ja520:36.4": "brahmadattassa rājino”.",
"ja520:37.1": "“Aparādho mahābrahme,",
"ja520:37.2": "brahmadattassa rājino;",
"ja520:37.3": "Arakkhitā jānapadā,",
"ja520:37.4": "adhammabalinā hatā.",
"ja520:38.1": "Rattiñhi corā khādanti,",
"ja520:38.2": "divā khādanti tuṇḍiyā;",
"ja520:38.3": "Raṭṭhasmiṃ kūṭarājassa,",
"ja520:38.4": "bahu adhammiko jano;",
"ja520:38.5": "Kathaṃ no asikosatthā,",
"ja520:38.6": "khīrapā haññate pajā”.",
"ja520:39.1": "“Evaṃ khajjatu pañcālo,",
"ja520:39.2": "hato yuddhe saputtako;",
"ja520:39.3": "Yathāhamajja khajjāmi,",
"ja520:39.4": "gāmikehi araññajo”.",
"ja520:40.1": "“Na sabbabhūtesu vidhenti rakkhaṃ,",
"ja520:40.2": "Rājāno maṇḍūka manussaloke;",
"ja520:40.3": "Nettāvatā rājā adhammacārī,",
"ja520:40.4": "Yaṃ tādisaṃ jīvamadeyyu dhaṅkā”.",
"ja520:41.1": "“Adhammarūpo vata brahmacārī,",
"ja520:41.2": "Anuppiyaṃ bhāsasi khattiyassa;",
"ja520:41.3": "Viluppamānāya puthuppajāya,",
"ja520:41.4": "Pūjesi rājaṃ paramappamādaṃ.",
"ja520:42.1": "Sace idaṃ brahme surajjakaṃ siyā,",
"ja520:42.2": "Phītaṃ raṭṭhaṃ muditaṃ vippasannaṃ;",
"ja520:42.3": "Bhutvā baliṃ aggapiṇḍañca kākā,",
"ja520:42.4": "Na mādisaṃ jīvamadeyyu dhaṅkā”ti. #2974",
"ja520:43.1": "Gandhatindukajātakaṃ dasamaṃ.",
"ja520:44.1": "Tiṃsanipātaṃ niṭṭhitaṃ.",
"ja520:45.1": "Tassuddānaṃ",
"ja520:46.1": "Kiṃchanda kumbha jayaddisa chaddanta,",
"ja520:46.2": "Atha paṇḍitasambhava sirakapi;",
"ja520:46.3": "Dakarakkhasa paṇḍaranāgavaro,",
"ja520:46.4": "Atha sambula tindukadevasutoti.",
"ja520:47.1": "Jātakapāḷiyā paṭhamo bhāgo niṭṭhito."
}