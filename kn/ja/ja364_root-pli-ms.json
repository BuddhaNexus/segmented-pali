{
"ja364:1.1": "Jātaka",
"ja364:2.1": "Pañcakanipāta",
"ja364:3.1": "Vaṇṇārohavagga",
"ja364:4.1": "4. Khajjopanakajātaka",
"ja364:5.1": "“Ko nu santamhi pajjote,",
"ja364:5.2": "aggipariyesanaṃ caraṃ;",
"ja364:5.3": "Addakkhi ratti khajjotaṃ,",
"ja364:5.4": "jātavedaṃ amaññatha.",
"ja364:6.1": "Svassa gomayacuṇṇāni,",
"ja364:6.2": "abhimatthaṃ tiṇāni ca;",
"ja364:6.3": "Viparītāya saññāya,",
"ja364:6.4": "nāsakkhi pajjaletave.",
"ja364:7.1": "Evampi anupāyena,",
"ja364:7.2": "atthaṃ na labhate migo;",
"ja364:7.3": "Visāṇato gavaṃ dohaṃ,",
"ja364:7.4": "yattha khīraṃ na vindati.",
"ja364:8.1": "Vividhehi upāyehi,",
"ja364:8.2": "atthaṃ papponti māṇavā;",
"ja364:8.3": "Niggahena amittānaṃ,",
"ja364:8.4": "mittānaṃ paggahena ca.",
"ja364:9.1": "Senāmokkhapalābhena,",
"ja364:9.2": "vallabhānaṃ nayena ca;",
"ja364:9.3": "Jagatiṃ jagatipālā,",
"ja364:9.4": "āvasanti vasundharan”ti.",
"ja364:10.1": "Khajjopanakajātakaṃ catutthaṃ."
}