{
"ja191:1.1": "Jātaka",
"ja191:2.1": "Dukanipāta",
"ja191:3.1": "Ruhakavagga",
"ja191:4.1": "1. Ruhakajātaka",
"ja191:5.1": "“Api ruhaka chinnāpi,",
"ja191:5.2": "jiyā sandhīyate puna;",
"ja191:5.3": "Sandhīyassu purāṇiyā,",
"ja191:5.4": "mā kodhassa vasaṃ gami”.",
"ja191:6.1": "“Vijjamānesu vākesu,",
"ja191:6.2": "vijjamānesu kārisu;",
"ja191:6.3": "Aññaṃ jiyaṃ karissāmi,",
"ja191:6.4": "alaññeva purāṇiyā”ti.",
"ja191:7.1": "Ruhakajātakaṃ paṭhamaṃ."
}