{
"ja404:1.1": "Jātaka",
"ja404:2.1": "Sattakanipāta",
"ja404:3.1": "Kukkuvagga",
"ja404:4.1": "9. Kapijātaka",
"ja404:5.1": "“Yattha verī nivasati,",
"ja404:5.2": "na vase tattha paṇḍito;",
"ja404:5.3": "Ekarattaṃ dirattaṃ vā,",
"ja404:5.4": "dukkhaṃ vasati verisu.",
"ja404:6.1": "Diso ve lahucittassa,",
"ja404:6.2": "posassānuvidhīyato;",
"ja404:6.3": "Ekassa kapino hetu,",
"ja404:6.4": "yūthassa anayo kato.",
"ja404:7.1": "Bālova paṇḍitamānī,",
"ja404:7.2": "yūthassa parihārako;",
"ja404:7.3": "Sacittassa vasaṃ gantvā,",
"ja404:7.4": "sayethāyaṃ yathā kapi.",
"ja404:8.1": "Na sādhu balavā bālo,",
"ja404:8.2": "yūthassa parihārako;",
"ja404:8.3": "Ahito bhavati ñātīnaṃ,",
"ja404:8.4": "sakuṇānaṃva cetako.",
"ja404:9.1": "Dhīrova balavā sādhu,",
"ja404:9.2": "yūthassa parihārako;",
"ja404:9.3": "Hito bhavati ñātīnaṃ,",
"ja404:9.4": "tidasānaṃva vāsavo.",
"ja404:10.1": "Yo ca sīlañca paññañca,",
"ja404:10.2": "sutañcattani passati;",
"ja404:10.3": "Ubhinnamatthaṃ carati,",
"ja404:10.4": "attano ca parassa ca.",
"ja404:11.1": "Tasmā tuleyya mattānaṃ,",
"ja404:11.2": "sīlapaññāsutāmiva;",
"ja404:11.3": "Gaṇaṃ vā parihare dhīro,",
"ja404:11.4": "eko vāpi paribbaje”ti.",
"ja404:12.1": "Kapijātakaṃ navamaṃ."
}