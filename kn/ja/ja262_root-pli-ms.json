{
"ja262:1.1": "Jātaka",
"ja262:2.1": "Tikanipāta",
"ja262:3.1": "Padumavagga",
"ja262:4.1": "2. Mudupāṇijātaka",
"ja262:5.1": "“Pāṇi ce muduko cassa,",
"ja262:5.2": "nāgo cassa sukārito;",
"ja262:5.3": "Andhakāro ca vasseyya,",
"ja262:5.4": "atha nūna tadā siyā”.",
"ja262:6.1": "“Analā mudusambhāsā,",
"ja262:6.2": "duppūrā tā nadīsamā;",
"ja262:6.3": "Sīdanti naṃ viditvāna,",
"ja262:6.4": "ārakā parivajjaye.",
"ja262:7.1": "Yaṃ etā upasevanti,",
"ja262:7.2": "chandasā vā dhanena vā;",
"ja262:7.3": "Jātavedova saṃ ṭhānaṃ,",
"ja262:7.4": "khippaṃ anudahanti nan”ti.",
"ja262:8.1": "Mudupāṇijātakaṃ dutiyaṃ."
}