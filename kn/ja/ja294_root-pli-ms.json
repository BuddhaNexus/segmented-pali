{
"ja294:1.1": "Jātaka",
"ja294:2.1": "Tikanipāta",
"ja294:3.1": "Kumbhavagga",
"ja294:4.1": "4. Jambukhādakajātaka",
"ja294:5.1": "“Koyaṃ bindussaro vaggu,",
"ja294:5.2": "saravantānamuttamo;",
"ja294:5.3": "Accuto jambusākhāya,",
"ja294:5.4": "moracchāpova kūjati”.",
"ja294:6.1": "“Kulaputtova jānāti,",
"ja294:6.2": "kulaputtaṃ pasaṃsituṃ;",
"ja294:6.3": "Byagghacchāpasarīvaṇṇa,",
"ja294:6.4": "bhuñja samma dadāmi te”.",
"ja294:7.1": "“Cirassaṃ vata passāmi,",
"ja294:7.2": "musāvādī samāgate;",
"ja294:7.3": "Vantādaṃ kuṇapādañca,",
"ja294:7.4": "aññamaññaṃ pasaṃsake”ti.",
"ja294:8.1": "Jambukhādakajātakaṃ catutthaṃ."
}