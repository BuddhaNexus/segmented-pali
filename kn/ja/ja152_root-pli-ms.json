{
"ja152:1.1": "Jātaka",
"ja152:2.1": "Dukanipāta",
"ja152:3.1": "Daḷhavagga",
"ja152:4.1": "2. Siṅgālajātaka",
"ja152:5.1": "“Asamekkhitakammantaṃ,",
"ja152:5.2": "turitābhinipātinaṃ;",
"ja152:5.3": "Sāni kammāni tappenti,",
"ja152:5.4": "uṇhaṃvajjhohitaṃ mukhe”.",
"ja152:6.1": "“Sīho ca sīhanādena,",
"ja152:6.2": "daddaraṃ abhinādayi;",
"ja152:6.3": "Sutvā sīhassa nigghosaṃ,",
"ja152:6.4": "siṅgālo daddare vasaṃ;",
"ja152:6.5": "Bhīto santāsamāpādi,",
"ja152:6.6": "hadayañcassa apphalī”ti.",
"ja152:7.1": "Siṅgālajātakaṃ dutiyaṃ."
}