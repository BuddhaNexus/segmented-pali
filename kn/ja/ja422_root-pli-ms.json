{
"ja422:1.1": "Jātaka",
"ja422:2.1": "Aṭṭhakanipāta",
"ja422:3.1": "Kaccānivagga",
"ja422:4.1": "6. Cetiyajātaka",
"ja422:5.1": "“Dhammo have hato hanti,",
"ja422:5.2": "Nāhato hanti kiñcanaṃ;",
"ja422:5.3": "Tasmā hi dhammaṃ na hane,",
"ja422:5.4": "Mā tvaṃ dhammo hato hani. #1",
"ja422:6.1": "Alikaṃ bhāsamānassa,",
"ja422:6.2": "apakkamanti devatā;",
"ja422:6.3": "Pūtikañca mukhaṃ vāti,",
"ja422:6.4": "sakaṭṭhānā ca dhaṃsati;",
"ja422:6.5": "Yo jānaṃ pucchito pañhaṃ,",
"ja422:6.6": "aññathā naṃ viyākare.",
"ja422:7.1": "Sace hi saccaṃ bhaṇasi,",
"ja422:7.2": "hohi rāja yathā pure;",
"ja422:7.3": "Musā ce bhāsase rāja,",
"ja422:7.4": "bhūmiyaṃ tiṭṭha cetiya. #2",
"ja422:8.1": "Akāle vassatī tassa,",
"ja422:8.2": "kāle tassa na vassati;",
"ja422:8.3": "Yo jānaṃ pucchito pañhaṃ,",
"ja422:8.4": "aññathā naṃ viyākare.",
"ja422:9.1": "Sace hi saccaṃ bhaṇasi,",
"ja422:9.2": "hohi rāja yathā pure;",
"ja422:9.3": "Musā ce bhāsase rāja,",
"ja422:9.4": "bhūmiṃ pavisa cetiya. #3",
"ja422:10.1": "Jivhā tassa dvidhā hoti,",
"ja422:10.2": "uragasseva disampati;",
"ja422:10.3": "Yo jānaṃ pucchito pañhaṃ,",
"ja422:10.4": "aññathā naṃ viyākare.",
"ja422:11.1": "Sace hi saccaṃ bhaṇasi,",
"ja422:11.2": "hohi rāja yathā pure;",
"ja422:11.3": "Musā ce bhāsase rāja,",
"ja422:11.4": "bhiyyo pavisa cetiya. #4",
"ja422:12.1": "Jivhā tassa na bhavati,",
"ja422:12.2": "macchasseva disampati;",
"ja422:12.3": "Yo jānaṃ pucchito pañhaṃ,",
"ja422:12.4": "aññathā naṃ viyākare.",
"ja422:13.1": "Sace hi saccaṃ bhaṇasi,",
"ja422:13.2": "hohi rāja yathā pure;",
"ja422:13.3": "Musā ce bhāsase rāja,",
"ja422:13.4": "bhiyyo pavisa cetiya. #5",
"ja422:14.1": "Thiyova tassa jāyanti,",
"ja422:14.2": "na pumā jāyare kule;",
"ja422:14.3": "Yo jānaṃ pucchito pañhaṃ,",
"ja422:14.4": "aññathā naṃ viyākare.",
"ja422:15.1": "Sace hi saccaṃ bhaṇasi,",
"ja422:15.2": "hohi rāja yathā pure;",
"ja422:15.3": "Musā ce bhāsase rāja,",
"ja422:15.4": "bhiyyo pavisa cetiya. #6",
"ja422:16.1": "Puttā tassa na bhavanti,",
"ja422:16.2": "pakkamanti disodisaṃ;",
"ja422:16.3": "Yo jānaṃ pucchito pañhaṃ,",
"ja422:16.4": "aññathā naṃ viyākare.",
"ja422:17.1": "Sace hi saccaṃ bhaṇasi,",
"ja422:17.2": "hohi rāja yathā pure;",
"ja422:17.3": "Musā ce bhāsase rāja,",
"ja422:17.4": "bhiyyo pavisa cetiya”. #7",
"ja422:18.1": "“Sa rājā isinā satto,",
"ja422:18.2": "antalikkhacaro pure;",
"ja422:18.3": "Pāvekkhi pathaviṃ cecco,",
"ja422:18.4": "hīnatto patva pariyāyaṃ.",
"ja422:19.1": "Tasmā hi chandāgamanaṃ,",
"ja422:19.2": "Nappasaṃsanti paṇḍitā;",
"ja422:19.3": "Aduṭṭhacitto bhāseyya,",
"ja422:19.4": "Giraṃ saccūpasaṃhitanti”. #8",
"ja422:20.1": "Cetiyajātakaṃ chaṭṭhaṃ."
}