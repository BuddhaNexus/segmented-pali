{
"ja254:1.1": "Jātaka",
"ja254:2.1": "Tikanipāta",
"ja254:3.1": "Saṅkappavagga",
"ja254:4.1": "4. Kuṇḍakakucchisindhavajātaka",
"ja254:5.1": "“Bhutvā tiṇaparighāsaṃ,",
"ja254:5.2": "bhutvā ācāmakuṇḍakaṃ;",
"ja254:5.3": "Etaṃ te bhojanaṃ āsi,",
"ja254:5.4": "kasmā dāni na bhuñjasi”.",
"ja254:6.1": "“Yattha posaṃ na jānanti,",
"ja254:6.2": "jātiyā vinayena vā;",
"ja254:6.3": "Bahuṃ tattha mahābrahme,",
"ja254:6.4": "api ācāmakuṇḍakaṃ.",
"ja254:7.1": "Tvañca kho maṃ pajānāsi,",
"ja254:7.2": "yādisāyaṃ hayuttamo;",
"ja254:7.3": "Jānanto jānamāgamma,",
"ja254:7.4": "na te bhakkhāmi kuṇḍakan”ti.",
"ja254:8.1": "Kuṇḍakakucchisindhavajātakaṃ catutthaṃ."
}