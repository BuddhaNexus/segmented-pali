{
"ja132:1.1": "Jātaka",
"ja132:2.1": "Ekakanipāta",
"ja132:3.1": "Asampadānavagga",
"ja132:4.1": "2. Bhīrukajātaka",
"ja132:5.1": "“Kusalūpadese dhitiyā daḷhāya ca,",
"ja132:5.2": "Anivattitattābhayabhīrutāya ca;",
"ja132:5.3": "Na rakkhasīnaṃ vasamāgamimhase,",
"ja132:5.4": "Sa sotthibhāvo mahatā bhayena me”ti.",
"ja132:6.1": "Bhīrukajātakaṃ dutiyaṃ."
}