{
"ja126:1.1": "Jātaka",
"ja126:2.1": "Ekakanipāta",
"ja126:3.1": "Kusanāḷivagga",
"ja126:4.1": "6. Asilakkhaṇajātaka",
"ja126:5.1": "“Tadevekassa kalyāṇaṃ,",
"ja126:5.2": "tadevekassa pāpakaṃ;",
"ja126:5.3": "Tasmā sabbaṃ na kalyāṇaṃ,",
"ja126:5.4": "sabbaṃ vāpi na pāpakan”ti.",
"ja126:6.1": "Asilakkhaṇajātakaṃ chaṭṭhaṃ."
}