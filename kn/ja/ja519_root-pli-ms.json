{
"ja519:1.1": "Jātaka",
"ja519:2.1": "Tiṃsanipāta",
"ja519:3.1": "Kiṃchandavagga",
"ja519:4.1": "9. Sambulājātaka",
"ja519:5.1": "“Kā vedhamānā girikandarāyaṃ,",
"ja519:5.2": "Ekā tuvaṃ tiṭṭhasi saṃhitūru;",
"ja519:5.3": "Puṭṭhāsi me pāṇipameyyamajjhe,",
"ja519:5.4": "Akkhāhi me nāmañca bandhave ca.",
"ja519:6.1": "Obhāsayaṃ vanaṃ rammaṃ,",
"ja519:6.2": "sīhabyagghanisevitaṃ;",
"ja519:6.3": "Kā vā tvamasi kalyāṇi,",
"ja519:6.4": "kassa vā tvaṃ sumajjhime;",
"ja519:6.5": "Abhivādemi taṃ bhadde,",
"ja519:6.6": "dānavāhaṃ namatthu te”.",
"ja519:7.1": "“Yo putto kāsirājassa,",
"ja519:7.2": "sotthisenoti taṃ vidū;",
"ja519:7.3": "Tassāhaṃ sambulā bhariyā,",
"ja519:7.4": "evaṃ jānāhi dānava;",
"ja519:7.5": "Abhivādemi taṃ bhante,",
"ja519:7.6": "sambulāhaṃ namatthu te.",
"ja519:8.1": "Vedehaputto bhaddante,",
"ja519:8.2": "vane vasati āturo;",
"ja519:8.3": "Tamahaṃ rogasammattaṃ,",
"ja519:8.4": "ekā ekaṃ upaṭṭhahaṃ.",
"ja519:9.1": "Ahañca vanamuñchāya,",
"ja519:9.2": "Madhumaṃsaṃ migābilaṃ;",
"ja519:9.3": "Yadāharāmi taṃ bhakkho,",
"ja519:9.4": "Tassa nūnajja nādhati”.",
"ja519:10.1": "“Kiṃ vane rājaputtena,",
"ja519:10.2": "āturena karissasi;",
"ja519:10.3": "Sambule pariciṇṇena,",
"ja519:10.4": "ahaṃ bhattā bhavāmi te”.",
"ja519:11.1": "“Sokaṭṭāya durattāya,",
"ja519:11.2": "kiṃ rūpaṃ vijjate mama;",
"ja519:11.3": "Aññaṃ pariyesa bhaddante,",
"ja519:11.4": "abhirūpataraṃ mayā”.",
"ja519:12.1": "“Ehimaṃ girimāruyha,",
"ja519:12.2": "bhariyā me catussatā;",
"ja519:12.3": "Tāsaṃ tvaṃ pavarā hohi,",
"ja519:12.4": "sabbakāmasamiddhinī.",
"ja519:13.1": "Nūna tārakavaṇṇābhe,",
"ja519:13.2": "yaṃ kiñci manasicchasi;",
"ja519:13.3": "Sabbaṃ taṃ pacuraṃ mayhaṃ,",
"ja519:13.4": "ramassvajja mayā saha.",
"ja519:14.1": "No ce tuvaṃ maheseyyaṃ,",
"ja519:14.2": "Sambule kārayissasi;",
"ja519:14.3": "Alaṃ tvaṃ pātarāsāya,",
"ja519:14.4": "Paṇhe bhakkhā bhavissasi”.",
"ja519:15.1": "Tañca sattajaṭo luddo,",
"ja519:15.2": "kaḷāro purisādako;",
"ja519:15.3": "Vane nāthaṃ apassantiṃ,",
"ja519:15.4": "sambulaṃ aggahī bhuje.",
"ja519:16.1": "Adhipannā pisācena,",
"ja519:16.2": "luddenāmisacakkhunā;",
"ja519:16.3": "Sā ca sattuvasaṃ pattā,",
"ja519:16.4": "patimevānusocati.",
"ja519:17.1": "“Na me idaṃ tathā dukkhaṃ,",
"ja519:17.2": "yaṃ maṃ khādeyya rakkhaso;",
"ja519:17.3": "Yañca me ayyaputtassa,",
"ja519:17.4": "mano hessati aññathā.",
"ja519:18.1": "Na santi devā pavasanti nūna,",
"ja519:18.2": "Na hi nūna santi idha lokapālā;",
"ja519:18.3": "Sahasā karontānamasaññatānaṃ,",
"ja519:18.4": "Na hi nūna santi paṭisedhitāro”.",
"ja519:19.1": "“Itthīnamesā pavarā yasassinī,",
"ja519:19.2": "Santā samā aggirivuggatejā;",
"ja519:19.3": "Tañce tuvaṃ rakkhasādesi kaññaṃ,",
"ja519:19.4": "Muddhā ca hi sattadhā te phaleyya;",
"ja519:19.5": "Mā tvaṃ dahī muñca patibbatāya”.",
"ja519:20.1": "Sā ca assamamāgacchi,",
"ja519:20.2": "pamuttā purisādakā;",
"ja519:20.3": "Nīḷaṃ paḷinaṃ sakuṇīva,",
"ja519:20.4": "gatasiṅgaṃva ālayaṃ.",
"ja519:21.1": "Sā tattha paridevesi,",
"ja519:21.2": "rājaputtī yasassinī;",
"ja519:21.3": "Sambulā utumattakkhā,",
"ja519:21.4": "vane nāthaṃ apassantī.",
"ja519:22.1": "Samaṇe brāhmaṇe vande,",
"ja519:22.2": "sampannacaraṇe ise;",
"ja519:22.3": "Rājaputtaṃ apassantī,",
"ja519:22.4": "“tumhaṃmhi saraṇaṃ gatā”.",
"ja519:23.1": "Vande sīhe ca byagghe ca,",
"ja519:23.2": "ye ca aññe vane migā;",
"ja519:23.3": "Rājaputtaṃ apassantī,",
"ja519:23.4": "“tumhaṃmhi saraṇaṃ gatā”.",
"ja519:24.1": "Tiṇā latāni osadhyo,",
"ja519:24.2": "pabbatāni vanāni ca;",
"ja519:24.3": "Rājaputtaṃ apassantī,",
"ja519:24.4": "“tumhaṃmhi saraṇaṃ gatā”.",
"ja519:25.1": "Vande indīvarīsāmaṃ,",
"ja519:25.2": "rattiṃ nakkhattamāliniṃ;",
"ja519:25.3": "Rājaputtaṃ apassantī,",
"ja519:25.4": "“tumhaṃmhi saraṇaṃ gatā”.",
"ja519:26.1": "Vande bhāgīrathiṃ gaṅgaṃ,",
"ja519:26.2": "savantīnaṃ paṭiggahaṃ;",
"ja519:26.3": "Rājaputtaṃ apassantī,",
"ja519:26.4": "“tumhaṃmhi saraṇaṃ gatā”.",
"ja519:27.1": "Vande ahaṃ pabbatarājaseṭṭhaṃ,",
"ja519:27.2": "Himavantaṃ siluccayaṃ;",
"ja519:27.3": "Rājaputtaṃ apassantī,",
"ja519:27.4": "“Tumhaṃmhi saraṇaṃ gatā”.",
"ja519:28.1": "“Atisāyaṃ vatāgacchi,",
"ja519:28.2": "rājaputti yasassini;",
"ja519:28.3": "Kena nujja samāgacchi,",
"ja519:28.4": "ko te piyataro mayā”.",
"ja519:29.1": "“Idaṃ khohaṃ tadāvocaṃ,",
"ja519:29.2": "gahitā tena sattunā;",
"ja519:29.3": "Na me idaṃ tathā dukkhaṃ,",
"ja519:29.4": "yaṃ maṃ khādeyya rakkhaso;",
"ja519:29.5": "Yañca me ayyaputtassa,",
"ja519:29.6": "mano hessati aññathā”.",
"ja519:30.1": "“Corīnaṃ bahubuddhīnaṃ,",
"ja519:30.2": "yāsu saccaṃ sudullabhaṃ;",
"ja519:30.3": "Thīnaṃ bhāvo durājāno,",
"ja519:30.4": "macchassevodake gataṃ”.",
"ja519:31.1": "“Tathā maṃ saccaṃ pāletu,",
"ja519:31.2": "pālayissati ce mamaṃ;",
"ja519:31.3": "Yathāhaṃ nābhijānāmi,",
"ja519:31.4": "aññaṃ piyataraṃ tayā;",
"ja519:31.5": "Etena saccavajjena,",
"ja519:31.6": "byādhi te vūpasammatu”.",
"ja519:32.1": "“Ye kuñjarā sattasatā uḷārā,",
"ja519:32.2": "Rakkhanti rattindivamuyyutāvudhā;",
"ja519:32.3": "Dhanuggahānañca satāni soḷasa,",
"ja519:32.4": "Kathaṃvidhe passasi bhadde sattavo”.",
"ja519:33.1": "“Alaṅkatāyo padumuttarattacā,",
"ja519:33.2": "Virāgitā passati haṃsagaggarā;",
"ja519:33.3": "Tāsaṃ suṇitvā mitagītavāditaṃ,",
"ja519:33.4": "Na dāni me tāta tathā yathā pure.",
"ja519:34.1": "Suvaṇṇasaṅkaccadharā suviggahā,",
"ja519:34.2": "Alaṅkatā mānusiyaccharūpamā;",
"ja519:34.3": "Senopiyā tāta aninditaṅgiyo,",
"ja519:34.4": "Khattiyakaññā paṭilobhayanti naṃ.",
"ja519:35.1": "Sace ahaṃ tāta tathā yathā pure,",
"ja519:35.2": "Patiṃ tamuñchāya punā vane bhare;",
"ja519:35.3": "Sammānaye maṃ na ca maṃ vimānaye,",
"ja519:35.4": "Itopi me tāta tato varaṃ siyā.",
"ja519:36.1": "Yamannapāne vipulasmi ohite,",
"ja519:36.2": "Nārī vimaṭṭhābharaṇā alaṅkatā;",
"ja519:36.3": "Sabbaṅgupetā patino ca appiyā,",
"ja519:36.4": "Abajjha tassā maraṇaṃ tato varaṃ.",
"ja519:37.1": "Api ce daliddā kapaṇā anāḷhiyā,",
"ja519:37.2": "Kaṭādutīyā patino ca sā piyā;",
"ja519:37.3": "Sabbaṅgupetāyapi appiyāya,",
"ja519:37.4": "Ayameva seyyā kapaṇāpi yā piyā”.",
"ja519:38.1": "“Sudullabhitthī purisassa yā hitā,",
"ja519:38.2": "Bhattitthiyā dullabho yo hito ca;",
"ja519:38.3": "Hitā ca te sīlavatī ca bhariyā,",
"ja519:38.4": "Janinda dhammaṃ cara sambulāya”.",
"ja519:39.1": "“Sace tuvaṃ vipule laddhabhoge,",
"ja519:39.2": "Issāvatiṇṇā maraṇaṃ upesi;",
"ja519:39.3": "Ahañca te bhadde imā rājakaññā,",
"ja519:39.4": "Sabbe te vacanakarā bhavāmā”ti.",
"ja519:40.1": "Sambulājātakaṃ navamaṃ."
}