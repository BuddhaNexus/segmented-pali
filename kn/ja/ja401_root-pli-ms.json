{
"ja401:1.1": "Jātaka",
"ja401:2.1": "Sattakanipāta",
"ja401:3.1": "Kukkuvagga",
"ja401:4.1": "6. Paṇṇakajātaka",
"ja401:5.1": "“Paṇṇakaṃ tikhiṇadhāraṃ,",
"ja401:5.2": "Asiṃ sampannapāyinaṃ;",
"ja401:5.3": "Parisāyaṃ puriso gilati,",
"ja401:5.4": "Kiṃ dukkarataraṃ tato;",
"ja401:5.5": "Yadaññaṃ dukkaraṃ ṭhānaṃ,",
"ja401:5.6": "Taṃ me akkhāhi pucchito”.",
"ja401:6.1": "“Gileyya puriso lobhā,",
"ja401:6.2": "asiṃ sampannapāyinaṃ;",
"ja401:6.3": "Yo ca vajjā dadāmīti,",
"ja401:6.4": "taṃ dukkarataraṃ tato;",
"ja401:6.5": "Sabbaññaṃ sukaraṃ ṭhānaṃ,",
"ja401:6.6": "evaṃ jānāhi maddava”.",
"ja401:7.1": "“Byākāsi āyuro pañhaṃ,",
"ja401:7.2": "atthaṃ dhammassa kovido;",
"ja401:7.3": "Pukkusaṃ dāni pucchāmi,",
"ja401:7.4": "kiṃ dukkarataraṃ tato;",
"ja401:7.5": "Yadaññaṃ dukkaraṃ ṭhānaṃ,",
"ja401:7.6": "taṃ me akkhāhi pucchito”.",
"ja401:8.1": "“Na vācamupajīvanti,",
"ja401:8.2": "aphalaṃ giramudīritaṃ;",
"ja401:8.3": "Yo ca datvā avākayirā,",
"ja401:8.4": "taṃ dukkarataraṃ tato;",
"ja401:8.5": "Sabbaññaṃ sukaraṃ ṭhānaṃ,",
"ja401:8.6": "evaṃ jānāhi maddava”.",
"ja401:9.1": "“Byākāsi pukkuso pañhaṃ,",
"ja401:9.2": "atthaṃ dhammassa kovido;",
"ja401:9.3": "Senakaṃ dāni pucchāmi,",
"ja401:9.4": "kiṃ dukkarataraṃ tato;",
"ja401:9.5": "Yadaññaṃ dukkaraṃ ṭhānaṃ,",
"ja401:9.6": "taṃ me akkhāhi pucchito”.",
"ja401:10.1": "“Dadeyya puriso dānaṃ,",
"ja401:10.2": "appaṃ vā yadi vā bahuṃ;",
"ja401:10.3": "Yo ca datvā nānutappe,",
"ja401:10.4": "taṃ dukkarataraṃ tato;",
"ja401:10.5": "Sabbaññaṃ sukaraṃ ṭhānaṃ,",
"ja401:10.6": "evaṃ jānāhi maddava”.",
"ja401:11.1": "“Byākāsi āyuro pañhaṃ,",
"ja401:11.2": "atho pukkusaporiso;",
"ja401:11.3": "Sabbe pañhe atibhoti,",
"ja401:11.4": "yathā bhāsati senako”ti.",
"ja401:12.1": "Paṇṇakajātakaṃ chaṭṭhaṃ."
}