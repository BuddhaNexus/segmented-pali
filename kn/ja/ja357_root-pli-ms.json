{
"ja357:1.1": "Jātaka",
"ja357:2.1": "Pañcakanipāta",
"ja357:3.1": "Maṇikuṇḍalavagga",
"ja357:4.1": "7. Laṭukikajātaka",
"ja357:5.1": "“Vandāmi taṃ kuñjara saṭṭhihāyanaṃ,",
"ja357:5.2": "Āraññakaṃ yūthapatiṃ yasassiṃ;",
"ja357:5.3": "Pakkhehi taṃ pañjalikaṃ karomi,",
"ja357:5.4": "Mā me vadhī puttake dubbalāya”.",
"ja357:6.1": "“Vandāmi taṃ kuñjara ekacāriṃ,",
"ja357:6.2": "Āraññakaṃ pabbatasānugocaraṃ;",
"ja357:6.3": "Pakkhehi taṃ pañjalikaṃ karomi,",
"ja357:6.4": "Mā me vadhī puttake dubbalāya”.",
"ja357:7.1": "“Vadhissāmi te laṭukike puttakāni,",
"ja357:7.2": "Kiṃ me tuvaṃ kāhasi dubbalāsi;",
"ja357:7.3": "Sataṃ sahassānipi tādisīnaṃ,",
"ja357:7.4": "Vāmena pādena papothayeyyaṃ”.",
"ja357:8.1": "“Na heva sabbattha balena kiccaṃ,",
"ja357:8.2": "Balañhi bālassa vadhāya hoti;",
"ja357:8.3": "Karissāmi te nāgarājā anatthaṃ,",
"ja357:8.4": "Yo me vadhī puttake dubbalāya”.",
"ja357:9.1": "“Kākañca passa laṭukikaṃ,",
"ja357:9.2": "Maṇḍūkaṃ nīlamakkhikaṃ;",
"ja357:9.3": "Ete nāgaṃ aghātesuṃ,",
"ja357:9.4": "Passa verassa verinaṃ;",
"ja357:9.5": "Tasmā hi veraṃ na kayirātha,",
"ja357:9.6": "Appiyenapi kenacī”ti.",
"ja357:10.1": "Laṭukikajātakaṃ sattamaṃ."
}