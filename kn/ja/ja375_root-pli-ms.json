{
"ja375:1.1": "Jātaka",
"ja375:2.1": "Pañcakanipāta",
"ja375:3.1": "Aḍḍhavagga",
"ja375:4.1": "5. Kapotajātaka",
"ja375:5.1": "“Idāni khomhi sukhito arogo,",
"ja375:5.2": "Nikkaṇṭako nippatito kapoto;",
"ja375:5.3": "Kāhāmi dānī hadayassa tuṭṭhiṃ,",
"ja375:5.4": "Tathāhimaṃ maṃsasākaṃ baleti”.",
"ja375:6.1": "“Kāyaṃ balākā sikhinī,",
"ja375:6.2": "corī laṅghipitāmahā;",
"ja375:6.3": "Oraṃ balāke āgaccha,",
"ja375:6.4": "caṇḍo me vāyaso sakhā”.",
"ja375:7.1": "“Alañhi te jagghitāye,",
"ja375:7.2": "Mamaṃ disvāna edisaṃ;",
"ja375:7.3": "Vilūnaṃ sūdaputtena,",
"ja375:7.4": "Piṭṭhamaṇḍena makkhitaṃ”.",
"ja375:8.1": "“Sunhāto suvilittosi,",
"ja375:8.2": "annapānena tappito;",
"ja375:8.3": "Kaṇṭhe ca te veḷuriyo,",
"ja375:8.4": "agamā nu kajaṅgalaṃ”.",
"ja375:9.1": "“Mā te mitto amitto vā,",
"ja375:9.2": "agamāsi kajaṅgalaṃ;",
"ja375:9.3": "Piñchāni tattha lāyitvā,",
"ja375:9.4": "kaṇṭhe bandhanti vaṭṭanaṃ”.",
"ja375:10.1": "“Punapāpajjasī samma,",
"ja375:10.2": "Sīlañhi tava tādisaṃ;",
"ja375:10.3": "Na hi mānusakā bhogā,",
"ja375:10.4": "Subhuñjā honti pakkhinā”ti. #841",
"ja375:11.1": "Kapotajātakaṃ pañcamaṃ.",
"ja375:11.2": "Aḍḍhavaggo tatiyo.",
"ja375:12.1": "Tassuddānaṃ",
"ja375:13.1": "Atha vaṇṇa sasīla hiri labhate,",
"ja375:13.2": "Sumukhā visa sāḷiyamittavaro;",
"ja375:13.3": "Atha cakka palāsa sarāja sato,",
"ja375:13.4": "Yava bāla kapotaka pannarasāti.",
"ja375:14.1": "Atha vagguddānaṃ",
"ja375:15.1": "Jīnañca vaṇṇaṃ asamaṃvaguppari,",
"ja375:15.2": "Sudesitā jātakanti santi vīsati;",
"ja375:15.3": "Mahesino brahmacarittamutta-",
"ja375:15.4": "Mavoca gāthā atthavatī subyañjanāti.",
"ja375:16.1": "Pañcakanipātaṃ niṭṭhitaṃ."
}