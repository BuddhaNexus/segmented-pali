{
"ja468:1.1": "Jātaka",
"ja468:2.1": "Dvādasakanipāta",
"ja468:3.1": "Cūḷakuṇālavagga",
"ja468:4.1": "5. Janasandhajātaka",
"ja468:5.1": "“Dasa khalu imāni ṭhānāni,",
"ja468:5.2": "Yāni pubbe akaritvā;",
"ja468:5.3": "Sa pacchā manutappati,",
"ja468:5.4": "Iccevāha janasandho.",
"ja468:6.1": "Aladdhā vittaṃ tappati,",
"ja468:6.2": "pubbe asamudānitaṃ;",
"ja468:6.3": "Na pubbe dhanamesissaṃ,",
"ja468:6.4": "iti pacchānutappati.",
"ja468:7.1": "Sakyarūpaṃ pure santaṃ,",
"ja468:7.2": "mayā sippaṃ na sikkhitaṃ;",
"ja468:7.3": "Kicchā vutti asippassa,",
"ja468:7.4": "iti pacchānutappati.",
"ja468:8.1": "Kūṭavedī pure āsiṃ,",
"ja468:8.2": "pisuṇo piṭṭhimaṃsiko;",
"ja468:8.3": "Caṇḍo ca pharuso cāpi,",
"ja468:8.4": "iti pacchānutappati.",
"ja468:9.1": "Pāṇātipātī pure āsiṃ,",
"ja468:9.2": "luddo cāpi anāriyo;",
"ja468:9.3": "Bhūtānaṃ nāpacāyissaṃ,",
"ja468:9.4": "iti pacchānutappati.",
"ja468:10.1": "Bahūsu vata santīsu,",
"ja468:10.2": "anāpādāsu itthisu;",
"ja468:10.3": "Paradāraṃ asevissaṃ,",
"ja468:10.4": "iti pacchānutappati.",
"ja468:11.1": "Bahumhi vata santamhi,",
"ja468:11.2": "annapāne upaṭṭhite;",
"ja468:11.3": "Na pubbe adadaṃ dānaṃ,",
"ja468:11.4": "iti pacchānutappati.",
"ja468:12.1": "Mātaraṃ pitarañcāpi,",
"ja468:12.2": "jiṇṇakaṃ gatayobbanaṃ;",
"ja468:12.3": "Pahu santo na posissaṃ,",
"ja468:12.4": "iti pacchānutappati.",
"ja468:13.1": "Ācariyamanusatthāraṃ,",
"ja468:13.2": "sabbakāmarasāharaṃ;",
"ja468:13.3": "Pitaraṃ atimaññissaṃ,",
"ja468:13.4": "iti pacchānutappati.",
"ja468:14.1": "Samaṇe brāhmaṇe cāpi,",
"ja468:14.2": "sīlavante bahussute;",
"ja468:14.3": "Na pubbe payirupāsissaṃ,",
"ja468:14.4": "iti pacchānutappati.",
"ja468:15.1": "Sādhu hoti tapo ciṇṇo,",
"ja468:15.2": "santo ca payirupāsito;",
"ja468:15.3": "Na ca pubbe tapo ciṇṇo,",
"ja468:15.4": "iti pacchānutappati.",
"ja468:16.1": "Yo ca etāni ṭhānāni,",
"ja468:16.2": "yoniso paṭipajjati;",
"ja468:16.3": "Karaṃ purisakiccāni,",
"ja468:16.4": "sa pacchā nānutappatī”ti.",
"ja468:17.1": "Janasandhajātakaṃ pañcamaṃ."
}