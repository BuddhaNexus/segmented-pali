{
"ja255:1.1": "Jātaka",
"ja255:2.1": "Tikanipāta",
"ja255:3.1": "Saṅkappavagga",
"ja255:4.1": "5. Sukajātaka",
"ja255:5.1": "“Yāva so mattamaññāsi,",
"ja255:5.2": "bhojanasmiṃ vihaṅgamo;",
"ja255:5.3": "Tāva addhānamāpādi,",
"ja255:5.4": "mātarañca aposayi.",
"ja255:6.1": "Yato ca kho bahutaraṃ,",
"ja255:6.2": "bhojanaṃ ajjhavāhari;",
"ja255:6.3": "Tato tattheva saṃsīdi,",
"ja255:6.4": "amattaññū hi so ahu.",
"ja255:7.1": "Tasmā mattaññutā sādhu,",
"ja255:7.2": "bhojanasmiṃ agiddhatā;",
"ja255:7.3": "Amattaññū hi sīdanti,",
"ja255:7.4": "mattaññū ca na sīdare”ti.",
"ja255:8.1": "Sukajātakaṃ pañcamaṃ."
}