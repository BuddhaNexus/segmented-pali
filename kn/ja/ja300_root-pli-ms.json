{
"ja300:1.1": "Jātaka",
"ja300:2.1": "Tikanipāta",
"ja300:3.1": "Kumbhavagga",
"ja300:4.1": "10. Vakajātaka",
"ja300:5.1": "“Parapāṇarodhā jīvanto,",
"ja300:5.2": "maṃsalohitabhojano;",
"ja300:5.3": "Vako vataṃ samādāya,",
"ja300:5.4": "upapajji uposathaṃ.",
"ja300:6.1": "Tassa sakko vataññāya,",
"ja300:6.2": "ajarūpenupāgami;",
"ja300:6.3": "Vītatapo ajjhappatto,",
"ja300:6.4": "bhañji lohitapo tapaṃ.",
"ja300:7.1": "Evameva idhekacce,",
"ja300:7.2": "Samādānamhi dubbalā;",
"ja300:7.3": "Lahuṃ karonti attānaṃ,",
"ja300:7.4": "Vakova ajakāraṇā”ti. #501",
"ja300:8.1": "Vakajātakaṃ dasamaṃ.",
"ja300:8.2": "Kumbhavaggo pañcamo.",
"ja300:9.1": "Tassuddānaṃ",
"ja300:10.1": "Varakumbha supattasirivhayano,",
"ja300:10.2": "Sucisammata bindusaro cusabho;",
"ja300:10.3": "Saritaṃpati caṇḍi jarākapinā,",
"ja300:10.4": "Atha makkaṭiyā vakakena dasāti.",
"ja300:11.1": "Atha vagguddānaṃ",
"ja300:12.1": "Saṅkappo padumo ceva,",
"ja300:12.2": "Udapānena tatiyaṃ;",
"ja300:12.3": "Abbhantaraṃ ghaṭabhedaṃ,",
"ja300:12.4": "Tikanipātamhilaṅkatanti.",
"ja300:13.1": "Tikanipātaṃ niṭṭhitaṃ."
}