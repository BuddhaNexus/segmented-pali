{
"ja115:1.1": "Jātaka",
"ja115:2.1": "Ekakanipāta",
"ja115:3.1": "Haṃcivagga",
"ja115:4.1": "5. Anusāsikajātaka",
"ja115:5.1": "“Yāyañña manusāsati,",
"ja115:5.2": "sayaṃ loluppacārinī;",
"ja115:5.3": "Sāyaṃ vipakkhikā seti,",
"ja115:5.4": "hatā cakkena sāsikāti.",
"ja115:6.1": "Anusāsikajātakaṃ pañcamaṃ."
}