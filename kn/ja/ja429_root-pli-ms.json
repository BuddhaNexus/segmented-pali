{
"ja429:1.1": "Jātaka",
"ja429:2.1": "Navakanipāta",
"ja429:3.1": "Gijjhavagga",
"ja429:4.1": "3. Mahāsuvajātaka",
"ja429:5.1": "“Dumo yadā hoti phalūpapanno,",
"ja429:5.2": "Bhuñjanti naṃ vihaṅgamā sampatantā;",
"ja429:5.3": "Khīṇanti ñatvāna dumaṃ phalaccaye,",
"ja429:5.4": "Disodisaṃ yanti tato vihaṅgamā.",
"ja429:6.1": "Cara cārikaṃ lohitatuṇḍa māmari,",
"ja429:6.2": "Kiṃ tvaṃ suva sukkhadumamhi jhāyasi;",
"ja429:6.3": "Tadiṅgha maṃ brūhi vasantasannibha,",
"ja429:6.4": "Kasmā suva sukkhadumaṃ na riñcasi”.",
"ja429:7.1": "“Ye ve sakhīnaṃ sakhāro bhavanti,",
"ja429:7.2": "Pāṇaccaye dukkhasukhesu haṃsa;",
"ja429:7.3": "Khīṇaṃ akhīṇampi na taṃ jahanti,",
"ja429:7.4": "Santo sataṃ dhammamanussarantā.",
"ja429:8.1": "Sohaṃ sataṃ aññatarosmi haṃsa,",
"ja429:8.2": "Ñātī ca me hoti sakhā ca rukkho;",
"ja429:8.3": "Taṃ nussahe jīvikattho pahātuṃ,",
"ja429:8.4": "Khīṇanti ñatvāna na hesa dhammo”.",
"ja429:9.1": "“Sādhu sakkhi kataṃ hoti,",
"ja429:9.2": "metti saṃsati santhavo;",
"ja429:9.3": "Sacetaṃ dhammaṃ rocesi,",
"ja429:9.4": "pāsaṃsosi vijānataṃ.",
"ja429:10.1": "So te suva varaṃ dammi,",
"ja429:10.2": "pattayāna vihaṅgama;",
"ja429:10.3": "Varaṃ varassu vakkaṅga,",
"ja429:10.4": "yaṃ kiñci manasicchasi”.",
"ja429:11.1": "“Varañca me haṃsa bhavaṃ dadeyya,",
"ja429:11.2": "Ayañca rukkho punarāyuṃ labhetha;",
"ja429:11.3": "So sākhavā phalimā saṃvirūḷho,",
"ja429:11.4": "Madhutthiko tiṭṭhatu sobhamāno”.",
"ja429:12.1": "“Taṃ passa samma phalimaṃ uḷāraṃ,",
"ja429:12.2": "Sahāva te hotu udumbarena;",
"ja429:12.3": "So sākhavā phalimā saṃvirūḷho,",
"ja429:12.4": "Madhutthiko tiṭṭhatu sobhamāno”.",
"ja429:13.1": "“Evaṃ sakka sukhī hohi,",
"ja429:13.2": "saha sabbehi ñātibhi;",
"ja429:13.3": "Yathāhamajja sukhito,",
"ja429:13.4": "disvāna saphalaṃ dumaṃ”.",
"ja429:14.1": "“Suvassa ca varaṃ datvā,",
"ja429:14.2": "katvāna saphalaṃ dumaṃ;",
"ja429:14.3": "Pakkāmi saha bhariyāya,",
"ja429:14.4": "devānaṃ nandanaṃ vanan”ti.",
"ja429:15.1": "Mahāsuvajātakaṃ tatiyaṃ."
}