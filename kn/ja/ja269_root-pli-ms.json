{
"ja269:1.1": "Jātaka",
"ja269:2.1": "Tikanipāta",
"ja269:3.1": "Padumavagga",
"ja269:4.1": "9. Sujātajātaka",
"ja269:5.1": "“Na hi vaṇṇena sampannā,",
"ja269:5.2": "mañjukā piyadassanā;",
"ja269:5.3": "Kharavācā piyā hoti,",
"ja269:5.4": "asmiṃ loke paramhi ca.",
"ja269:6.1": "Nanu passasimaṃ kāḷiṃ,",
"ja269:6.2": "dubbaṇṇaṃ tilakāhataṃ;",
"ja269:6.3": "Kokilaṃ saṇhavācena,",
"ja269:6.4": "bahūnaṃ pāṇinaṃ piyaṃ.",
"ja269:7.1": "Tasmā sakhilavācassa,",
"ja269:7.2": "mantabhāṇī anuddhato;",
"ja269:7.3": "Atthaṃ dhammañca dīpeti,",
"ja269:7.4": "madhuraṃ tassa bhāsitan”ti.",
"ja269:8.1": "Sujātajātakaṃ navamaṃ."
}