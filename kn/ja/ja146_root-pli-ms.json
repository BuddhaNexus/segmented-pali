{
"ja146:1.1": "Jātaka",
"ja146:2.1": "Ekakanipāta",
"ja146:3.1": "Kakaṇṭakavagga",
"ja146:4.1": "6. Samuddakākajātaka",
"ja146:5.1": "“Api nu hanukā santā,",
"ja146:5.2": "mukhañca parisussati;",
"ja146:5.3": "Oramāma na pārema,",
"ja146:5.4": "pūrateva mahodadhī”ti.",
"ja146:6.1": "Samuddakākajātakaṃ chaṭṭhaṃ."
}