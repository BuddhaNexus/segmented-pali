{
"ja256:1.1": "Jātaka",
"ja256:2.1": "Tikanipāta",
"ja256:3.1": "Saṅkappavagga",
"ja256:4.1": "6. Jarūdapānajātaka",
"ja256:5.1": "“Jarūdapānaṃ khaṇamānā,",
"ja256:5.2": "vāṇijā udakatthikā;",
"ja256:5.3": "Ajjhagamuṃ ayasaṃ lohaṃ,",
"ja256:5.4": "tipusīsañca vāṇijā;",
"ja256:5.5": "Rajataṃ jātarūpañca,",
"ja256:5.6": "muttā veḷuriyā bahū.",
"ja256:6.1": "Te ca tena asantuṭṭhā,",
"ja256:6.2": "bhiyyo bhiyyo akhāṇisuṃ;",
"ja256:6.3": "Te tatthāsīviso ghoro,",
"ja256:6.4": "tejassī tejasā hani.",
"ja256:7.1": "Tasmā khaṇe nātikhaṇe,",
"ja256:7.2": "Atikhātaṃ hi pāpakaṃ;",
"ja256:7.3": "Khātena ca dhanaṃ laddhaṃ,",
"ja256:7.4": "Atikhātena nāsitan”ti.",
"ja256:8.1": "Jarūdapānajātakaṃ chaṭṭhaṃ."
}