{
"ja324:1.1": "Jātaka",
"ja324:2.1": "Catukkanipāta",
"ja324:3.1": "Kuṭidūsakavagga",
"ja324:4.1": "4. Cammasāṭakajātaka",
"ja324:5.1": "“Kalyāṇarūpo vatayaṃ catuppado,",
"ja324:5.2": "Subhaddako ceva supesalo ca;",
"ja324:5.3": "Yo brāhmaṇaṃ jātimantūpapannaṃ,",
"ja324:5.4": "Apacāyati meṇḍavaro yasassī”.",
"ja324:6.1": "“Mā brāhmaṇa ittaradassanena,",
"ja324:6.2": "Vissāsamāpajji catuppadassa;",
"ja324:6.3": "Daḷhappahāraṃ abhikaṅkhamāno,",
"ja324:6.4": "Avasakkatī dassati suppahāraṃ”.",
"ja324:7.1": "Ūruṭṭhi bhaggaṃ pavaṭṭito khāribhāro,",
"ja324:7.2": "Sabbañca bhaṇḍaṃ brāhmaṇassa bhinnaṃ;",
"ja324:7.3": "Ubhopi bāhā paggayha kandati,",
"ja324:7.4": "“Abhidhāvatha haññate brahmacārī.",
"ja324:8.1": "Evaṃ so nihato seti,",
"ja324:8.2": "yo apūjaṃ pasaṃsati;",
"ja324:8.3": "Yathāhamajja pahato,",
"ja324:8.4": "hato meṇḍena dummatī”ti.",
"ja324:9.1": "Cammasāṭakajātakaṃ catutthaṃ."
}