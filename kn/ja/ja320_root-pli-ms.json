{
"ja320:1.1": "Jātaka",
"ja320:2.1": "Catukkanipāta",
"ja320:3.1": "Pucimandavagga",
"ja320:4.1": "10. Succajajātaka",
"ja320:5.1": "“Succajaṃ vata naccaji,",
"ja320:5.2": "vācāya adadaṃ giriṃ;",
"ja320:5.3": "Kiṃ hitassa cajantassa,",
"ja320:5.4": "vācāya adada pabbataṃ”.",
"ja320:6.1": "“Yañhi kayirā tañhi vade,",
"ja320:6.2": "yaṃ na kayirā na taṃ vade;",
"ja320:6.3": "Akarontaṃ bhāsamānaṃ,",
"ja320:6.4": "parijānanti paṇḍitā”.",
"ja320:7.1": "“Rājaputta namo tyatthu,",
"ja320:7.2": "sacce dhamme ṭhito casi;",
"ja320:7.3": "Yassa te byasanaṃ patto,",
"ja320:7.4": "saccasmiṃ ramate mano”.",
"ja320:8.1": "“Yā daliddī daliddassa,",
"ja320:8.2": "aḍḍhā aḍḍhassa kittima;",
"ja320:8.3": "Sā hissa paramā bhariyā,",
"ja320:8.4": "sahiraññassa itthiyo”ti.",
"ja320:9.1": "Succajajātakaṃ dasamaṃ.",
"ja320:9.2": "Pucimandavaggo dutiyo.",
"ja320:10.1": "Tassuddānaṃ",
"ja320:11.1": "Atha cora sakassapa khantīvaro,",
"ja320:11.2": "Dujjīvitatā ca varā pharusā;",
"ja320:11.3": "Atha sasa matañca vasanta sukhaṃ,",
"ja320:11.4": "Succajaṃvatanaccajinā ca dasāti."
}