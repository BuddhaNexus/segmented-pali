{
"ja4:1.1": "Jātaka",
"ja4:2.1": "Ekakanipāta",
"ja4:3.1": "Apaṇṇakavagga",
"ja4:4.1": "4. Cūḷaseṭṭhijātaka",
"ja4:5.1": "“Appakenapi medhāvī,",
"ja4:5.2": "pābhatena vicakkhaṇo;",
"ja4:5.3": "Samuṭṭhāpeti attānaṃ,",
"ja4:5.4": "aṇuṃ aggiṃva sandhaman”ti.",
"ja4:6.1": "Cūḷaseṭṭhijātakaṃ catutthaṃ."
}