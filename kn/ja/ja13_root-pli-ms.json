{
"ja13:1.1": "Jātaka",
"ja13:2.1": "Ekakanipāta",
"ja13:3.1": "Sīlavagga",
"ja13:4.1": "3. Kaṇḍijātaka",
"ja13:5.1": "“Dhiratthu kaṇḍinaṃ sallaṃ,",
"ja13:5.2": "purisaṃ gāḷhavedhinaṃ;",
"ja13:5.3": "Dhiratthu taṃ janapadaṃ,",
"ja13:5.4": "yatthitthī pariṇāyikā;",
"ja13:5.5": "Te cāpi dhikkitā sattā,",
"ja13:5.6": "ye itthīnaṃ vasaṅgatā”ti.",
"ja13:6.1": "Kaṇḍijātakaṃ tatiyaṃ."
}