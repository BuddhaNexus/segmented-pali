{
"ja273:1.1": "Jātaka",
"ja273:2.1": "Tikanipāta",
"ja273:3.1": "Udapānavagga",
"ja273:4.1": "3. Kacchapajātaka",
"ja273:5.1": "“Ko nu uddhitabhattova,",
"ja273:5.2": "pūrahatthova brāhmaṇo;",
"ja273:5.3": "Kahaṃ nu bhikkhaṃ acari,",
"ja273:5.4": "kaṃ saddhaṃ upasaṅkami”.",
"ja273:6.1": "“Ahaṃ kapismi dummedho,",
"ja273:6.2": "anāmāsāni āmasiṃ;",
"ja273:6.3": "Tvaṃ maṃ mocaya bhaddaṃ te,",
"ja273:6.4": "mutto gaccheyya pabbataṃ”.",
"ja273:7.1": "“Kacchapā kassapā honti,",
"ja273:7.2": "Koṇḍaññā honti makkaṭā;",
"ja273:7.3": "Muñca kassapa koṇḍaññaṃ,",
"ja273:7.4": "Kataṃ methunakaṃ tayā”ti.",
"ja273:8.1": "Kacchapajātakaṃ tatiyaṃ."
}