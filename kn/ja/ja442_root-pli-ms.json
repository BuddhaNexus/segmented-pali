{
"ja442:1.1": "Jātaka",
"ja442:2.1": "Dasakanipāta",
"ja442:3.1": "Catudvāravagga",
"ja442:4.1": "4. Saṅkhajātaka",
"ja442:5.1": "“Bahussuto sutadhammosi saṅkha,",
"ja442:5.2": "Diṭṭhā tayā samaṇabrāhmaṇā ca;",
"ja442:5.3": "Athakkhaṇe dassayase vilāpaṃ,",
"ja442:5.4": "Añño nu ko te paṭimantako mayā”.",
"ja442:6.1": "“Subbhū subhā suppaṭimukkakambu,",
"ja442:6.2": "Paggayha sovaṇṇamayāya pātiyā;",
"ja442:6.3": "‘Bhuñjassu bhattaṃ’ iti maṃ vadeti,",
"ja442:6.4": "Saddhāvittā",
"ja442:6.5": "Tamahaṃ noti brūmi”.",
"ja442:7.1": "“Etādisaṃ brāhmaṇa disvāna yakkhaṃ,",
"ja442:7.2": "Puccheyya poso sukhamāsisāno;",
"ja442:7.3": "Uṭṭhehi naṃ pañjalikābhipuccha,",
"ja442:7.4": "Devī nusi tvaṃ uda mānusī nu”.",
"ja442:8.1": "“Yaṃ tvaṃ sukhenābhisamekkhase maṃ,",
"ja442:8.2": "Bhuñjassu bhattaṃ iti maṃ vadesi;",
"ja442:8.3": "Pucchāmi taṃ nāri mahānubhāve,",
"ja442:8.4": "Devī nusi tvaṃ uda mānusī nu”.",
"ja442:9.1": "“Devī ahaṃ saṅkha mahānubhāvā,",
"ja442:9.2": "Idhāgatā sāgaravārimajjhe;",
"ja442:9.3": "Anukampikā no ca paduṭṭhacittā,",
"ja442:9.4": "Taveva atthāya idhāgatāsmi.",
"ja442:10.1": "Idhannapānaṃ sayanāsanañca,",
"ja442:10.2": "Yānāni nānāvividhāni saṅkha;",
"ja442:10.3": "Sabbassa tyāhaṃ paṭipādayāmi,",
"ja442:10.4": "Yaṃ kiñci tuyhaṃ manasābhipatthitaṃ”.",
"ja442:11.1": "“Yaṃ kiñci yiṭṭhañca hutañca mayhaṃ,",
"ja442:11.2": "Sabbassa no issarā tvaṃ sugatte;",
"ja442:11.3": "Susoṇi subbhamu suvilaggamajjhe,",
"ja442:11.4": "Kissa me kammassa ayaṃ vipāko”.",
"ja442:12.1": "“Ghamme pathe brāhmaṇa ekabhikkhuṃ,",
"ja442:12.2": "Ugghaṭṭapādaṃ tasitaṃ kilantaṃ;",
"ja442:12.3": "Paṭipādayī saṅkha upāhanāni,",
"ja442:12.4": "Sā dakkhiṇā kāmaduhā tavajja”.",
"ja442:13.1": "“Sā hotu nāvā phalakūpapannā,",
"ja442:13.2": "Anavassutā erakavātayuttā;",
"ja442:13.3": "Aññassa yānassa na hettha bhūmi,",
"ja442:13.4": "Ajjeva maṃ moḷiniṃ pāpayassu”.",
"ja442:14.1": "“Sā tattha vittā sumanā patītā,",
"ja442:14.2": "Nāvaṃ sucittaṃ abhinimminitvā;",
"ja442:14.3": "Ādāya saṅkhaṃ purisena saddhiṃ,",
"ja442:14.4": "Upānayī nagaraṃ sādhuramman”ti.",
"ja442:15.1": "Saṅkhajātakaṃ catutthaṃ."
}