{
"ja206:1.1": "Jātaka",
"ja206:2.1": "Dukanipāta",
"ja206:3.1": "Nataṃdaḷhavagga",
"ja206:4.1": "6. Kuruṅgamigajātaka",
"ja206:5.1": "“Iṅgha vaṭṭamayaṃ pāsaṃ,",
"ja206:5.2": "chinda dantehi kacchapa;",
"ja206:5.3": "Ahaṃ tathā karissāmi,",
"ja206:5.4": "yathā nehiti luddako”.",
"ja206:6.1": "“Kacchapo pāvisī vāriṃ,",
"ja206:6.2": "kuruṅgo pāvisī vanaṃ;",
"ja206:6.3": "Satapatto dumaggamhā,",
"ja206:6.4": "dūre putte apānayī”ti.",
"ja206:7.1": "Kuruṅgamigajātakaṃ chaṭṭhaṃ."
}