{
"ja297:1.1": "Jātaka",
"ja297:2.1": "Tikanipāta",
"ja297:3.1": "Kumbhavagga",
"ja297:4.1": "7. Kāmavilāpajātaka",
"ja297:5.1": "“Ucce sakuṇa ḍemāna,",
"ja297:5.2": "pattayāna vihaṅgama;",
"ja297:5.3": "Vajjāsi kho tvaṃ vāmūruṃ,",
"ja297:5.4": "ciraṃ kho sā karissati.",
"ja297:6.1": "Idaṃ kho sā na jānāti,",
"ja297:6.2": "asiṃ sattiñca oḍḍitaṃ;",
"ja297:6.3": "Sā caṇḍī kāhati kodhaṃ,",
"ja297:6.4": "taṃ me tapati no idaṃ.",
"ja297:7.1": "Esa uppalasannāho,",
"ja297:7.2": "nikkhañcussīsakohitaṃ;",
"ja297:7.3": "Kāsikañca muduṃ vatthaṃ,",
"ja297:7.4": "tappetu dhanikā piyā”ti.",
"ja297:8.1": "Kāmavilāpajātakaṃ sattamaṃ."
}