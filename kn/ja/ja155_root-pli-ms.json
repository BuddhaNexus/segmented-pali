{
"ja155:1.1": "Jātaka",
"ja155:2.1": "Dukanipāta",
"ja155:3.1": "Daḷhavagga",
"ja155:4.1": "5. Bhaggajātaka",
"ja155:5.1": "“Jīva vassasataṃ bhagga,",
"ja155:5.2": "aparāni ca vīsatiṃ;",
"ja155:5.3": "Mā maṃ pisācā khādantu,",
"ja155:5.4": "jīva tvaṃ sarado sataṃ”.",
"ja155:6.1": "“Tvampi vassasataṃ jīva,",
"ja155:6.2": "aparāni ca vīsatiṃ;",
"ja155:6.3": "Visaṃ pisācā khādantu,",
"ja155:6.4": "jīva tvaṃ sarado satan”ti.",
"ja155:7.1": "Bhaggajātakaṃ pañcamaṃ."
}