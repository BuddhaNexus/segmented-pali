{
"ja325:1.1": "Jātaka",
"ja325:2.1": "Catukkanipāta",
"ja325:3.1": "Kuṭidūsakavagga",
"ja325:4.1": "5. Godharājajātaka",
"ja325:5.1": "“Samaṇaṃ taṃ maññamāno,",
"ja325:5.2": "upagacchimasaññataṃ;",
"ja325:5.3": "So maṃ daṇḍena pāhāsi,",
"ja325:5.4": "yathā assamaṇo tathā”.",
"ja325:6.1": "“Kiṃ te jaṭāhi dummedha,",
"ja325:6.2": "kiṃ te ajinasāṭiyā;",
"ja325:6.3": "Abbhantaraṃ te gahanaṃ,",
"ja325:6.4": "bāhiraṃ parimajjasi”.",
"ja325:7.1": "“Ehi godha nivattassu,",
"ja325:7.2": "bhuñja sālīnamodanaṃ;",
"ja325:7.3": "Telaṃ loṇañca me atthi,",
"ja325:7.4": "pahūtaṃ mayha pipphali.",
"ja325:8.1": "Esa bhiyyo pavekkhāmi,",
"ja325:8.2": "vammikaṃ sataporisaṃ;",
"ja325:8.3": "Telaṃ loṇañca kittesi,",
"ja325:8.4": "ahitaṃ mayha pipphalī”ti.",
"ja325:9.1": "Godharājajātakaṃ pañcamaṃ."
}