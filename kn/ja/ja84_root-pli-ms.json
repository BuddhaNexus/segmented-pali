{
"ja84:1.1": "Jātaka",
"ja84:2.1": "Ekakanipāta",
"ja84:3.1": "Apāyimhavagga",
"ja84:4.1": "4. Atthassadvārajātaka",
"ja84:5.1": "“Ārogyamicche paramañca lābhaṃ,",
"ja84:5.2": "Sīlañca vuddhānumataṃ sutañca;",
"ja84:5.3": "Dhammānuvattī ca alīnatā ca,",
"ja84:5.4": "Atthassa dvārā pamukhā chaḷete”ti.",
"ja84:6.1": "Atthassadvārajātakaṃ catutthaṃ."
}