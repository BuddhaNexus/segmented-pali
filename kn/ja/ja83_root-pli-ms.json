{
"ja83:1.1": "Jātaka",
"ja83:2.1": "Ekakanipāta",
"ja83:3.1": "Apāyimhavagga",
"ja83:4.1": "3. Kāḷakaṇṇijātaka",
"ja83:5.1": "“Mitto have sattapadena hoti,",
"ja83:5.2": "Sahāyo pana dvādasakena hoti;",
"ja83:5.3": "Māsaḍḍhamāsena ca ñāti hoti,",
"ja83:5.4": "Tatuttariṃ attasamopi hoti;",
"ja83:5.5": "Sohaṃ kathaṃ attasukhassa hetu,",
"ja83:5.6": "Cirasanthutaṃ kāḷakaṇṇiṃ jaheyyan”ti.",
"ja83:6.1": "Kāḷakaṇṇijātakaṃ tatiyaṃ"
}