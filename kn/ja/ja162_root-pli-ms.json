{
"ja162:1.1": "Jātaka",
"ja162:2.1": "Dukanipāta",
"ja162:3.1": "Santhavavagga",
"ja162:4.1": "2. Santhavajātaka",
"ja162:5.1": "“Na santhavasmā paramatthi pāpiyo,",
"ja162:5.2": "Yo santhavo kāpurisena hoti;",
"ja162:5.3": "Santappito sappinā pāyasena,",
"ja162:5.4": "Kicchākataṃ paṇṇakuṭiṃ adayhi.",
"ja162:6.1": "Na santhavasmā paramatthi seyyo,",
"ja162:6.2": "Yo santhavo sappurisena hoti;",
"ja162:6.3": "Sīhassa byagghassa ca dīpino ca,",
"ja162:6.4": "Sāmā mukhaṃ lehati santhavenā”ti.",
"ja162:7.1": "Santhavajātakaṃ dutiyaṃ."
}