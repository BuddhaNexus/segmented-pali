{
"ja278:1.1": "Jātaka",
"ja278:2.1": "Tikanipāta",
"ja278:3.1": "Udapānavagga",
"ja278:4.1": "8. Mahiṃsarājajātaka",
"ja278:5.1": "“Kimatthamabhisandhāya,",
"ja278:5.2": "lahucittassa dubbhino;",
"ja278:5.3": "Sabbakāmadadasseva,",
"ja278:5.4": "imaṃ dukkhaṃ titikkhasi.",
"ja278:6.1": "Siṅgena nihanāhetaṃ,",
"ja278:6.2": "padasā ca adhiṭṭhaha;",
"ja278:6.3": "Bhiyyo bālā pakujjheyyuṃ,",
"ja278:6.4": "no cassa paṭisedhako”.",
"ja278:7.1": "“Mamevāyaṃ maññamāno,",
"ja278:7.2": "aññepevaṃ karissati;",
"ja278:7.3": "Te naṃ tattha vadhissanti,",
"ja278:7.4": "sā me mutti bhavissatī”ti.",
"ja278:8.1": "Mahiṃsarājajātakaṃ aṭṭhamaṃ."
}