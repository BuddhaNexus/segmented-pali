{
"ja30:1.1": "Jātaka",
"ja30:2.1": "Ekakanipāta",
"ja30:3.1": "Kuruṅgavagga",
"ja30:4.1": "10. Munikajātaka",
"ja30:5.1": "“Mā munikassa pihayi,",
"ja30:5.2": "āturannāni bhuñjati;",
"ja30:5.3": "Appossukko bhusaṃ khāda,",
"ja30:5.4": "etaṃ dīghāyulakkhaṇan”ti.",
"ja30:6.1": "Munikajātakaṃ dasamaṃ.",
"ja30:6.2": "Kuruṅgavaggo tatiyo.",
"ja30:7.1": "Tassuddānaṃ",
"ja30:8.1": "Kuruṅgassa kukkuragojavaro,",
"ja30:8.2": "Puna vāḷavassasirivhayano;",
"ja30:8.3": "Mahiḷāmukhanāmanuññavaro,",
"ja30:8.4": "Vahate dhura munikena dasāti."
}