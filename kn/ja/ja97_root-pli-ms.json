{
"ja97:1.1": "Jātaka",
"ja97:2.1": "Ekakanipāta",
"ja97:3.1": "Littavagga",
"ja97:4.1": "7. Nāmasiddhijātaka",
"ja97:5.1": "“Jīvakañca mataṃ disvā,",
"ja97:5.2": "dhanapāliñca duggataṃ;",
"ja97:5.3": "Panthakañca vane mūḷhaṃ,",
"ja97:5.4": "pāpako punarāgato”ti.",
"ja97:6.1": "Nāmasiddhijātakaṃ sattamaṃ."
}