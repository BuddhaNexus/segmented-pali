{
"ja247:1.1": "Jātaka",
"ja247:2.1": "Dukanipāta",
"ja247:3.1": "Siṅgālavagga",
"ja247:4.1": "7. Pādañjalījātaka",
"ja247:5.1": "“Addhā pādañjalī sabbe,",
"ja247:5.2": "paññāya atirocati;",
"ja247:5.3": "Tathā hi oṭṭhaṃ bhañjati,",
"ja247:5.4": "uttariṃ nūna passati”.",
"ja247:6.1": "“Nāyaṃ dhammaṃ adhammaṃ vā,",
"ja247:6.2": "Atthānatthañca bujjhati;",
"ja247:6.3": "Aññatra oṭṭhanibbhogā,",
"ja247:6.4": "Nāyaṃ jānāti kiñcanan”ti.",
"ja247:7.1": "Pādañjalījātakaṃ sattamaṃ."
}