{
"ja173:1.1": "Jātaka",
"ja173:2.1": "Dukanipāta",
"ja173:3.1": "Kalyāṇavagga",
"ja173:4.1": "3. Makkaṭajātaka",
"ja173:5.1": "“Tāta māṇavako eso,",
"ja173:5.2": "tālamūlaṃ apassito;",
"ja173:5.3": "Agārakañcidaṃ atthi,",
"ja173:5.4": "handa demassagārakaṃ”.",
"ja173:6.1": "“Mā kho tvaṃ tāta pakkosi,",
"ja173:6.2": "dūseyya no agārakaṃ;",
"ja173:6.3": "Netādisaṃ mukhaṃ hoti,",
"ja173:6.4": "brāhmaṇassa susīlino”ti.",
"ja173:7.1": "Makkaṭajātakaṃ tatiyaṃ."
}