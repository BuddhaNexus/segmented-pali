{
"ja100:1.1": "Jātaka",
"ja100:2.1": "Ekakanipāta",
"ja100:3.1": "Littavagga",
"ja100:4.1": "10. Asātarūpajātaka",
"ja100:5.1": "“Asātaṃ sātarūpena,",
"ja100:5.2": "piyarūpena appiyaṃ;",
"ja100:5.3": "Dukkhaṃ sukhassa rūpena,",
"ja100:5.4": "pamattamativattatī”ti.",
"ja100:6.1": "Asātarūpajātakaṃ dasamaṃ.",
"ja100:6.2": "Littavaggo dasamo.",
"ja100:7.1": "Tassuddānaṃ",
"ja100:8.1": "Gilamakkhakutūhala mātukassā,",
"ja100:8.2": "Muninā ca aniccata pattavaraṃ;",
"ja100:8.3": "Dhanapālivaro atipaṇḍitako,",
"ja100:8.4": "Saparosahassaasātadasāti.",
"ja100:9.1": "Majjhimo paṇṇāsako."
}