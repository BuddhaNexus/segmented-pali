{
"ja280:1.1": "Jātaka",
"ja280:2.1": "Tikanipāta",
"ja280:3.1": "Udapānavagga",
"ja280:4.1": "10. Puṭadūsakajātaka",
"ja280:5.1": "“Addhā hi nūna migarājā,",
"ja280:5.2": "puṭakammassa kovido;",
"ja280:5.3": "Tathā hi puṭaṃ dūseti,",
"ja280:5.4": "aññaṃ nūna karissati”.",
"ja280:6.1": "“Na me mātā vā pitā vā,",
"ja280:6.2": "puṭakammassa kovido;",
"ja280:6.3": "Kataṃ kataṃ kho dūsema,",
"ja280:6.4": "evaṃ dhammamidaṃ kulaṃ”.",
"ja280:7.1": "“Yesaṃ vo ediso dhammo,",
"ja280:7.2": "Adhammo pana kīdiso;",
"ja280:7.3": "Mā vo dhammaṃ adhammaṃ vā,",
"ja280:7.4": "Addasāma kudācanan”ti.",
"ja280:8.1": "Puṭadūsakajātakaṃ dasamaṃ.",
"ja280:8.2": "Udapānavaggo tatiyo.",
"ja280:9.1": "Tassuddānaṃ",
"ja280:10.1": "Udapānavaraṃ vanabyaggha kapi,",
"ja280:10.2": "Sikhinī ca balāka ruciravaro;",
"ja280:10.3": "Sujanādhiparomakadūsa puna,",
"ja280:10.4": "Satapattavaro puṭakamma dasāti."
}