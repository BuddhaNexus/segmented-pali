{
"ja322:1.1": "Jātaka",
"ja322:2.1": "Catukkanipāta",
"ja322:3.1": "Kuṭidūsakavagga",
"ja322:4.1": "2. Duddubhajātaka",
"ja322:5.1": "“Duddubhāyati bhaddante,",
"ja322:5.2": "yasmiṃ dese vasāmahaṃ;",
"ja322:5.3": "Ahampetaṃ na jānāmi,",
"ja322:5.4": "kimetaṃ duddubhāyati”.",
"ja322:6.1": "“Beluvaṃ patitaṃ sutvā,",
"ja322:6.2": "duddubhanti saso javi;",
"ja322:6.3": "Sasassa vacanaṃ sutvā,",
"ja322:6.4": "santattā migavāhinī.",
"ja322:7.1": "Appatvā padaviññāṇaṃ,",
"ja322:7.2": "paraghosānusārino;",
"ja322:7.3": "Panādaparamā bālā,",
"ja322:7.4": "te honti parapattiyā.",
"ja322:8.1": "Ye ca sīlena sampannā,",
"ja322:8.2": "paññāyūpasame ratā;",
"ja322:8.3": "Ārakā viratā dhīrā,",
"ja322:8.4": "na honti parapattiyā”ti.",
"ja322:9.1": "Duddubhajātakaṃ dutiyaṃ."
}