{
"ja258:1.1": "Jātaka",
"ja258:2.1": "Tikanipāta",
"ja258:3.1": "Saṅkappavagga",
"ja258:4.1": "8. Mandhātujātaka",
"ja258:5.1": "“Yāvatā candimasūriyā,",
"ja258:5.2": "Pariharanti disā bhanti virocanā;",
"ja258:5.3": "Sabbeva dāsā mandhātu,",
"ja258:5.4": "Ye pāṇā pathavissitā.",
"ja258:6.1": "Na kahāpaṇavassena,",
"ja258:6.2": "titti kāmesu vijjati;",
"ja258:6.3": "Appassādā dukhā kāmā,",
"ja258:6.4": "iti viññāya paṇḍito.",
"ja258:7.1": "Api dibbesu kāmesu,",
"ja258:7.2": "ratiṃ so nādhigacchati;",
"ja258:7.3": "Taṇhakkhayarato hoti,",
"ja258:7.4": "sammāsambuddhasāvako”ti.",
"ja258:8.1": "Mandhātujātakaṃ aṭṭhamaṃ."
}