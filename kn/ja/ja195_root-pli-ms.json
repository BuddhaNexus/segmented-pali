{
"ja195:1.1": "Jātaka",
"ja195:2.1": "Dukanipāta",
"ja195:3.1": "Ruhakavagga",
"ja195:4.1": "5. Pabbatūpattharajātaka",
"ja195:5.1": "“Pabbatūpatthare ramme,",
"ja195:5.2": "jātā pokkharaṇī sivā;",
"ja195:5.3": "Taṃ siṅgālo apāpāyi,",
"ja195:5.4": "jānaṃ sīhena rakkhitaṃ”.",
"ja195:6.1": "“Pivanti ce mahārāja,",
"ja195:6.2": "sāpadāni mahānadiṃ;",
"ja195:6.3": "Na tena anadī hoti,",
"ja195:6.4": "khamassu yadi te piyā”ti.",
"ja195:7.1": "Pabbatūpattharajātakaṃ pañcamaṃ."
}