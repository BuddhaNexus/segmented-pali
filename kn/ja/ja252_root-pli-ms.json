{
"ja252:1.1": "Jātaka",
"ja252:2.1": "Tikanipāta",
"ja252:3.1": "Saṅkappavagga",
"ja252:4.1": "2. Tilamuṭṭhijātaka",
"ja252:5.1": "“Ajjāpi me taṃ manasi,",
"ja252:5.2": "yaṃ maṃ tvaṃ tilamuṭṭhiyā;",
"ja252:5.3": "Bāhāya maṃ gahetvāna,",
"ja252:5.4": "laṭṭhiyā anutāḷayi.",
"ja252:6.1": "Nanu jīvite na ramasi,",
"ja252:6.2": "yenāsi brāhmaṇāgato;",
"ja252:6.3": "Yaṃ maṃ bāhā gahetvāna,",
"ja252:6.4": "tikkhattuṃ anutāḷayi”.",
"ja252:7.1": "“Ariyo anariyaṃ kubbantaṃ,",
"ja252:7.2": "Yo daṇḍena nisedhati;",
"ja252:7.3": "Sāsanaṃ taṃ na taṃ veraṃ,",
"ja252:7.4": "Iti naṃ paṇḍitā vidū”ti.",
"ja252:8.1": "Tilamuṭṭhijātakaṃ dutiyaṃ."
}