{
"ja3:1.1": "Jātaka",
"ja3:2.1": "Ekakanipāta",
"ja3:3.1": "Apaṇṇakavagga",
"ja3:4.1": "3. Serivavāṇijajātaka",
"ja3:5.1": "“Idha ce naṃ virādhesi,",
"ja3:5.2": "saddhammassa niyāmataṃ;",
"ja3:5.3": "Ciraṃ tvaṃ anutappesi,",
"ja3:5.4": "serivāyaṃva vāṇijo”ti.",
"ja3:6.1": "Serivavāṇijajātakaṃ tatiyaṃ."
}