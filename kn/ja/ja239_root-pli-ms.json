{
"ja239:1.1": "Jātaka",
"ja239:2.1": "Dukanipāta",
"ja239:3.1": "Upāhanavagga",
"ja239:4.1": "9. Haritamaṇḍūkajātaka",
"ja239:5.1": "“Āsīvisampi maṃ santaṃ,",
"ja239:5.2": "Paviṭṭhaṃ kumināmukhaṃ;",
"ja239:5.3": "Ruccate haritāmātā,",
"ja239:5.4": "Yaṃ maṃ khādanti macchakā”.",
"ja239:6.1": "“Vilumpateva puriso,",
"ja239:6.2": "yāvassa upakappati;",
"ja239:6.3": "Yadā caññe vilumpanti,",
"ja239:6.4": "so vilutto vilumpatī”ti.",
"ja239:7.1": "Haritamaṇḍūkajātakaṃ navamaṃ."
}