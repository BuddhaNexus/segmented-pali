{
"ja406:1.1": "Jātaka",
"ja406:2.1": "Sattakanipāta",
"ja406:3.1": "Gandhāravagga",
"ja406:4.1": "1. Gandhārajātaka",
"ja406:5.1": "“Hitvā gāmasahassāni,",
"ja406:5.2": "paripuṇṇāni soḷasa;",
"ja406:5.3": "Koṭṭhāgārāni phītāni,",
"ja406:5.4": "sannidhiṃ dāni kubbasi”.",
"ja406:6.1": "“Hitvā gandhāravisayaṃ,",
"ja406:6.2": "pahūtadhanadhāriyaṃ;",
"ja406:6.3": "Pasāsanato nikkhanto,",
"ja406:6.4": "idha dāni pasāsasi”.",
"ja406:7.1": "“Dhammaṃ bhaṇāmi vedeha,",
"ja406:7.2": "adhammo me na ruccati;",
"ja406:7.3": "Dhammaṃ me bhaṇamānassa,",
"ja406:7.4": "na pāpamupalimpati”.",
"ja406:8.1": "“Yena kenaci vaṇṇena,",
"ja406:8.2": "paro labhati ruppanaṃ;",
"ja406:8.3": "Mahatthiyampi ce vācaṃ,",
"ja406:8.4": "na taṃ bhāseyya paṇḍito”.",
"ja406:9.1": "“Kāmaṃ ruppatu vā mā vā,",
"ja406:9.2": "bhusaṃva vikirīyatu;",
"ja406:9.3": "Dhammaṃ me bhaṇamānassa,",
"ja406:9.4": "na pāpamupalimpati”.",
"ja406:10.1": "“No ce assa sakā buddhi,",
"ja406:10.2": "vinayo vā susikkhito;",
"ja406:10.3": "Vane andhamahiṃsova,",
"ja406:10.4": "careyya bahuko jano.",
"ja406:11.1": "Yasmā ca panidhekacce,",
"ja406:11.2": "āceramhi susikkhitā;",
"ja406:11.3": "Tasmā vinītavinayā,",
"ja406:11.4": "caranti susamāhitā”ti.",
"ja406:12.1": "Gandhārajātakaṃ paṭhamaṃ."
}