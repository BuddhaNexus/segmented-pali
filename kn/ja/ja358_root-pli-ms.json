{
"ja358:1.1": "Jātaka",
"ja358:2.1": "Pañcakanipāta",
"ja358:3.1": "Maṇikuṇḍalavagga",
"ja358:4.1": "8. Cūḷadhammapālajātaka",
"ja358:5.1": "“Ahameva dūsiyā bhūnahatā,",
"ja358:5.2": "Rañño mahāpatāpassa;",
"ja358:5.3": "Etaṃ muñcatu dhammapālaṃ,",
"ja358:5.4": "Hatthe me deva chedehi.",
"ja358:6.1": "Ahameva dūsiyā bhūnahatā,",
"ja358:6.2": "Rañño mahāpatāpassa;",
"ja358:6.3": "Etaṃ muñcatu dhammapālaṃ,",
"ja358:6.4": "Pāde me deva chedehi.",
"ja358:7.1": "Ahameva dūsiyā bhūnahatā,",
"ja358:7.2": "Rañño mahāpatāpassa;",
"ja358:7.3": "Etaṃ muñcatu dhammapālaṃ,",
"ja358:7.4": "Sīsaṃ me deva chedehi.",
"ja358:8.1": "Na hi nūnimassa rañño,",
"ja358:8.2": "Mittāmaccā ca vijjare suhadā;",
"ja358:8.3": "Ye na vadanti rājānaṃ,",
"ja358:8.4": "Mā ghātayi orasaṃ puttaṃ.",
"ja358:9.1": "Na hi nūnimassa rañño,",
"ja358:9.2": "Ñātī mittā ca vijjare suhadā;",
"ja358:9.3": "Ye na vadanti rājānaṃ,",
"ja358:9.4": "Mā ghātayi atrajaṃ puttaṃ.",
"ja358:10.1": "Candanasārānulittā,",
"ja358:10.2": "Bāhā chijjanti dhammapālassa;",
"ja358:10.3": "Dāyādassa pathabyā,",
"ja358:10.4": "Pāṇā me deva rujjhantī”ti.",
"ja358:11.1": "Cūḷadhammapālajātakaṃ aṭṭhamaṃ."
}