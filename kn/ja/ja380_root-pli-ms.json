{
"ja380:1.1": "Jātaka",
"ja380:2.1": "Chakkanipāta",
"ja380:3.1": "Avāriyavagga",
"ja380:4.1": "5. Āsaṅkajātaka",
"ja380:5.1": "“Āsāvatī nāma latā,",
"ja380:5.2": "jātā cittalatāvane;",
"ja380:5.3": "Tassā vassasahassena,",
"ja380:5.4": "ekaṃ nibbattate phalaṃ.",
"ja380:6.1": "Taṃ devā payirupāsanti,",
"ja380:6.2": "tāva dūraphalaṃ satiṃ;",
"ja380:6.3": "Āsīseva tuvaṃ rāja,",
"ja380:6.4": "āsā phalavatī sukhā.",
"ja380:7.1": "Āsīsateva so pakkhī,",
"ja380:7.2": "āsīsateva so dijo;",
"ja380:7.3": "Tassa cāsā samijjhati,",
"ja380:7.4": "tāva dūragatā satī;",
"ja380:7.5": "Āsīseva tuvaṃ rāja,",
"ja380:7.6": "āsā phalavatī sukhā”.",
"ja380:8.1": "“Sampesi kho maṃ vācāya,",
"ja380:8.2": "na ca sampesi kammunā;",
"ja380:8.3": "Mālā sereyyakasseva,",
"ja380:8.4": "vaṇṇavantā agandhikā.",
"ja380:9.1": "Aphalaṃ madhuraṃ vācaṃ,",
"ja380:9.2": "yo mittesu pakubbati;",
"ja380:9.3": "Adadaṃ avissajaṃ bhogaṃ,",
"ja380:9.4": "sandhi tenassa jīrati.",
"ja380:10.1": "Yañhi kayirā tañhi vade,",
"ja380:10.2": "yaṃ na kayirā na taṃ vade;",
"ja380:10.3": "Akarontaṃ bhāsamānaṃ,",
"ja380:10.4": "parijānanti paṇḍitā.",
"ja380:11.1": "Balañca vata me khīṇaṃ,",
"ja380:11.2": "pātheyyañca na vijjati;",
"ja380:11.3": "Saṅke pāṇūparodhāya,",
"ja380:11.4": "handa dāni vajāmahaṃ”.",
"ja380:12.1": "“Etadeva hi me nāmaṃ,",
"ja380:12.2": "Yaṃ nāmasmi rathesabha;",
"ja380:12.3": "Āgamehi mahārāja,",
"ja380:12.4": "Pitaraṃ āmantayāmahan”ti.",
"ja380:13.1": "Āsaṅkajātakaṃ pañcamaṃ."
}