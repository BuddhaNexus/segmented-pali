{
"ja354:1.1": "Jātaka",
"ja354:2.1": "Pañcakanipāta",
"ja354:3.1": "Maṇikuṇḍalavagga",
"ja354:4.1": "4. Uragajātaka",
"ja354:5.1": "“Uragova tacaṃ jiṇṇaṃ,",
"ja354:5.2": "hitvā gacchati saṃ tanuṃ;",
"ja354:5.3": "Evaṃ sarīre nibbhoge,",
"ja354:5.4": "pete kālaṅkate sati.",
"ja354:6.1": "Ḍayhamāno na jānāti,",
"ja354:6.2": "ñātīnaṃ paridevitaṃ;",
"ja354:6.3": "Tasmā etaṃ na socāmi,",
"ja354:6.4": "gato so tassa yā gati”.",
"ja354:7.1": "“Anavhito tato āgā,",
"ja354:7.2": "ananuññāto ito gato;",
"ja354:7.3": "Yathāgato tathā gato,",
"ja354:7.4": "tattha kā paridevanā.",
"ja354:8.1": "Ḍayhamāno na jānāti,",
"ja354:8.2": "ñātīnaṃ paridevitaṃ;",
"ja354:8.3": "Tasmā etaṃ na socāmi,",
"ja354:8.4": "gato so tassa yā gati”.",
"ja354:9.1": "“Sace rode kisā assaṃ,",
"ja354:9.2": "tassā me kiṃ phalaṃ siyā;",
"ja354:9.3": "Ñātimittasuhajjānaṃ,",
"ja354:9.4": "bhiyyo no aratī siyā.",
"ja354:10.1": "Ḍayhamāno na jānāti,",
"ja354:10.2": "ñātīnaṃ paridevitaṃ;",
"ja354:10.3": "Tasmā etaṃ na socāmi,",
"ja354:10.4": "gato so tassa yā gati”.",
"ja354:11.1": "“Yathāpi dārako candaṃ,",
"ja354:11.2": "gacchantamanurodati;",
"ja354:11.3": "Evaṃsampadamevetaṃ,",
"ja354:11.4": "yo petamanusocati.",
"ja354:12.1": "Ḍayhamāno na jānāti,",
"ja354:12.2": "ñātīnaṃ paridevitaṃ;",
"ja354:12.3": "Tasmā etaṃ na socāmi,",
"ja354:12.4": "gato so tassa yā gati”.",
"ja354:13.1": "“Yathāpi udakakumbho,",
"ja354:13.2": "bhinno appaṭisandhiyo;",
"ja354:13.3": "Evaṃsampadamevetaṃ,",
"ja354:13.4": "yo petamanusocati.",
"ja354:14.1": "Ḍayhamāno na jānāti,",
"ja354:14.2": "ñātīnaṃ paridevitaṃ;",
"ja354:14.3": "Tasmā etaṃ na socāmi,",
"ja354:14.4": "gato so tassa yā gatī”ti.",
"ja354:15.1": "Uragajātakaṃ catutthaṃ."
}