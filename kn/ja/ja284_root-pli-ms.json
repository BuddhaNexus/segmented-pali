{
"ja284:1.1": "Jātaka",
"ja284:2.1": "Tikanipāta",
"ja284:3.1": "Abbhantaravagga",
"ja284:4.1": "4. Sirijātaka",
"ja284:5.1": "“Yaṃ ussukā saṃgharanti,",
"ja284:5.2": "alakkhikā bahuṃ dhanaṃ;",
"ja284:5.3": "Sippavanto asippā ca,",
"ja284:5.4": "lakkhivā tāni bhuñjati.",
"ja284:6.1": "Sabbattha katapuññassa,",
"ja284:6.2": "aticcaññeva pāṇino;",
"ja284:6.3": "Uppajjanti bahū bhogā,",
"ja284:6.4": "appanāyatanesupi.",
"ja284:7.1": "Kukkuṭo maṇayo daṇḍo,",
"ja284:7.2": "thiyo ca puññalakkhaṇā;",
"ja284:7.3": "Uppajjanti apāpassa,",
"ja284:7.4": "katapuññassa jantuno”ti.",
"ja284:8.1": "Sirijātakaṃ catutthaṃ."
}