{
"ja12:1.1": "Jātaka",
"ja12:2.1": "Ekakanipāta",
"ja12:3.1": "Sīlavagga",
"ja12:4.1": "2. Nigrodhamigajātaka",
"ja12:5.1": "“Nigrodhameva seveyya,",
"ja12:5.2": "na sākhamupasaṃvase;",
"ja12:5.3": "Nigrodhasmiṃ mataṃ seyyo,",
"ja12:5.4": "yañce sākhasmi jīvitan”ti.",
"ja12:6.1": "Nigrodhamigajātakaṃ dutiyaṃ."
}