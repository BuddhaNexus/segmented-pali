{
"ja123:1.1": "Jātaka",
"ja123:2.1": "Ekakanipāta",
"ja123:3.1": "Kusanāḷivagga",
"ja123:4.1": "3. Naṅgalīsajātaka",
"ja123:5.1": "“Asabbatthagāmiṃ vācaṃ,",
"ja123:5.2": "Bālo sabbattha bhāsati;",
"ja123:5.3": "Nāyaṃ dadhiṃ vedi na naṅgalīsaṃ,",
"ja123:5.4": "Dadhippayaṃ maññati naṅgalīsan”ti.",
"ja123:6.1": "Naṅgalīsajātakaṃ tatiyaṃ."
}