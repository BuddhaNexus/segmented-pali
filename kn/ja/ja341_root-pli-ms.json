{
"ja341:1.1": "Jātaka",
"ja341:2.1": "Catukkanipāta",
"ja341:3.1": "Cūḷakuṇālavagga",
"ja341:4.1": "1. Kaṇḍarījātaka",
"ja341:5.1": "Narānamārāmakarāsu nārisu,",
"ja341:5.2": "Anekacittāsu aniggahāsu ca;",
"ja341:5.3": "Sabbattha nāpītikarāpi ce siyā,",
"ja341:5.4": "Na vissase titthasamā hi nāriyo.",
"ja341:6.1": "“Yaṃ ve disvā kaṇḍarīkinnarānaṃ,",
"ja341:6.2": "Sabbitthiyo na ramanti agāre;",
"ja341:6.3": "Taṃ tādisaṃ maccaṃ cajitvā bhariyā,",
"ja341:6.4": "Aññaṃ disvā purisaṃ pīṭhasappiṃ”.",
"ja341:7.1": "“Bakassa ca bāvarikassa rañño,",
"ja341:7.2": "Accantakāmānugatassa bhariyā;",
"ja341:7.3": "Avācarī paṭṭhavasānugassa,",
"ja341:7.4": "Kaṃ vāpi itthī nāticare tadaññaṃ”.",
"ja341:8.1": "“Piṅgiyānī sabbalokissarassa,",
"ja341:8.2": "Rañño piyā brahmadattassa bhariyā;",
"ja341:8.3": "Avācarī paṭṭhavasānugassa,",
"ja341:8.4": "Taṃ vāpi sā nājjhagā kāmakāminī”ti.",
"ja341:9.1": "Kaṇḍarījātakaṃ paṭhamaṃ."
}