{
"ja362:1.1": "Jātaka",
"ja362:2.1": "Pañcakanipāta",
"ja362:3.1": "Vaṇṇārohavagga",
"ja362:4.1": "2. Sīlavīmaṃsajātaka",
"ja362:5.1": "“Sīlaṃ seyyo sutaṃ seyyo,",
"ja362:5.2": "iti me saṃsayo ahu;",
"ja362:5.3": "Sīlameva sutā seyyo,",
"ja362:5.4": "iti me natthi saṃsayo.",
"ja362:6.1": "Moghā jāti ca vaṇṇo ca,",
"ja362:6.2": "sīlameva kiruttamaṃ;",
"ja362:6.3": "Sīlena anupetassa,",
"ja362:6.4": "sutenattho na vijjati.",
"ja362:7.1": "Khattiyo ca adhammaṭṭho,",
"ja362:7.2": "vesso cādhammanissito;",
"ja362:7.3": "Te pariccajjubho loke,",
"ja362:7.4": "upapajjanti duggatiṃ.",
"ja362:8.1": "Khattiyā brāhmaṇā vessā,",
"ja362:8.2": "suddā caṇḍālapukkusā;",
"ja362:8.3": "Idha dhammaṃ caritvāna,",
"ja362:8.4": "bhavanti tidive samā.",
"ja362:9.1": "Na vedā samparāyāya,",
"ja362:9.2": "Na jāti nāpi bandhavā;",
"ja362:9.3": "Sakañca sīlaṃ saṃsuddhaṃ,",
"ja362:9.4": "Samparāyāya sukhāya cā”ti.",
"ja362:10.1": "Sīlavīmaṃsajātakaṃ dutiyaṃ."
}