{
"ja238:1.1": "Jātaka",
"ja238:2.1": "Dukanipāta",
"ja238:3.1": "Upāhanavagga",
"ja238:4.1": "8. Ekapadajātaka",
"ja238:5.1": "“Iṅgha ekapadaṃ tāta,",
"ja238:5.2": "anekatthapadassitaṃ;",
"ja238:5.3": "Kiñci saṅgāhikaṃ brūsi,",
"ja238:5.4": "yenatthe sādhayemase”.",
"ja238:6.1": "“Dakkheyyekapadaṃ tāta,",
"ja238:6.2": "anekatthapadassitaṃ;",
"ja238:6.3": "Tañca sīlena saññuttaṃ,",
"ja238:6.4": "khantiyā upapāditaṃ;",
"ja238:6.5": "Alaṃ mitte sukhāpetuṃ,",
"ja238:6.6": "amittānaṃ dukhāya cā”ti.",
"ja238:7.1": "Ekapadajātakaṃ aṭṭhamaṃ."
}