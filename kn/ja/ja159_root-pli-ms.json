{
"ja159:1.1": "Jātaka",
"ja159:2.1": "Dukanipāta",
"ja159:3.1": "Daḷhavagga",
"ja159:4.1": "9. Morajātaka",
"ja159:5.1": "“Udetayañ cakkhumā ekarājā,",
"ja159:5.2": "Harissavaṇṇo pathavippabhāso;",
"ja159:5.3": "Taṃ taṃ namassāmi harissavaṇṇaṃ pathavippabhāsaṃ,",
"ja159:5.4": "Tayājja guttā viharemu divasaṃ.",
"ja159:6.1": "Ye brāhmaṇā vedagū sabbadhamme,",
"ja159:6.2": "Te me namo te ca maṃ pālayantu;",
"ja159:6.3": "Namatthu buddhānaṃ namatthu bodhiyā,",
"ja159:6.4": "Namo vimuttānaṃ namo vimuttiyā;",
"ja159:6.5": "Imaṃ so parittaṃ katvā,",
"ja159:6.6": "Moro carati esanā.",
"ja159:7.1": "Apetayaṃ cakkhumā ekarājā,",
"ja159:7.2": "Harissavaṇṇo pathavippabhāso;",
"ja159:7.3": "Taṃ taṃ namassāmi harissavaṇṇaṃ pathavippabhāsaṃ,",
"ja159:7.4": "Tayājja guttā viharemu rattiṃ.",
"ja159:8.1": "Ye brāhmaṇā vedagū sabbadhamme,",
"ja159:8.2": "Te me namo te ca maṃ pālayantu;",
"ja159:8.3": "Namatthu buddhānaṃ namatthu bodhiyā,",
"ja159:8.4": "Namo vimuttānaṃ namo vimuttiyā;",
"ja159:8.5": "Imaṃ so parittaṃ katvā,",
"ja159:8.6": "Moro vāsamakappayī”ti.",
"ja159:9.1": "Morajātakaṃ navamaṃ."
}