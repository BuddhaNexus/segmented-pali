{
"ja204:1.1": "Jātaka",
"ja204:2.1": "Dukanipāta",
"ja204:3.1": "Nataṃdaḷhavagga",
"ja204:4.1": "4. Vīrakajātaka",
"ja204:5.1": "“Api vīraka passesi,",
"ja204:5.2": "sakuṇaṃ mañjubhāṇakaṃ;",
"ja204:5.3": "Mayūragīvasaṅkāsaṃ,",
"ja204:5.4": "patiṃ mayhaṃ saviṭṭhakaṃ”.",
"ja204:6.1": "“Udakathalacarassa pakkhino,",
"ja204:6.2": "Niccaṃ āmakamacchabhojino;",
"ja204:6.3": "Tassānukaraṃ saviṭṭhako,",
"ja204:6.4": "Sevāle paliguṇṭhito mato”ti.",
"ja204:7.1": "Vīrakajātakaṃ catutthaṃ."
}