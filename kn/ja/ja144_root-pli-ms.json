{
"ja144:1.1": "Jātaka",
"ja144:2.1": "Ekakanipāta",
"ja144:3.1": "Kakaṇṭakavagga",
"ja144:4.1": "4. Naṅguṭṭhajātaka",
"ja144:5.1": "“Bahumpetaṃ asabbhi jātaveda,",
"ja144:5.2": "Yaṃ taṃ vāladhinābhipūjayāma;",
"ja144:5.3": "Maṃsārahassa natthajja maṃsaṃ,",
"ja144:5.4": "Naṅguṭṭhampi bhavaṃ paṭiggahātū”ti.",
"ja144:6.1": "Naṅguṭṭhajātakaṃ catutthaṃ."
}