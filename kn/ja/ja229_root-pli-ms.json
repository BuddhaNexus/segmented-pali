{
"ja229:1.1": "Jātaka",
"ja229:2.1": "Dukanipāta",
"ja229:3.1": "Kāsāvavagga",
"ja229:4.1": "9. Palāyitajātaka",
"ja229:5.1": "“Gajaggameghehi hayaggamālibhi,",
"ja229:5.2": "Rathūmijātehi sarābhivassebhi;",
"ja229:5.3": "Tharuggahāvaṭṭadaḷhappahāribhi,",
"ja229:5.4": "Parivāritā takkasilā samantato.",
"ja229:6.1": "Abhidhāvatha cūpadhāvatha ca,",
"ja229:6.2": "Vividhā vināditā vadantibhi;",
"ja229:6.3": "Vattatajja tumulo ghoso yathā,",
"ja229:6.4": "Vijjulatā jaladharassa gajjato”ti.",
"ja229:7.1": "Palāyitajātakaṃ navamaṃ."
}