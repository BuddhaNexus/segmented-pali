{
"ja216:1.1": "Jātaka",
"ja216:2.1": "Dukanipāta",
"ja216:3.1": "Bīraṇathambhavagga",
"ja216:4.1": "6. Macchajātaka",
"ja216:5.1": "“Na māyamaggi tapati,",
"ja216:5.2": "na sūlo sādhutacchito;",
"ja216:5.3": "Yañca maṃ maññate macchī,",
"ja216:5.4": "aññaṃ so ratiyā gato.",
"ja216:6.1": "So maṃ dahati rāgaggi,",
"ja216:6.2": "cittaṃ cūpatapeti maṃ;",
"ja216:6.3": "Jālino muñcathāyirā maṃ,",
"ja216:6.4": "na kāme haññate kvacī”ti.",
"ja216:7.1": "Macchajātakaṃ chaṭṭhaṃ."
}