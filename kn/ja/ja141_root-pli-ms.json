{
"ja141:1.1": "Jātaka",
"ja141:2.1": "Ekakanipāta",
"ja141:3.1": "Kakaṇṭakavagga",
"ja141:4.1": "1. Godhajātaka",
"ja141:5.1": "“Na pāpajanasaṃsevī,",
"ja141:5.2": "accantasukhamedhati;",
"ja141:5.3": "Godhākulaṃ kakaṇṭāva,",
"ja141:5.4": "kaliṃ pāpeti attanan”ti.",
"ja141:6.1": "Godhajātakaṃ paṭhamaṃ."
}