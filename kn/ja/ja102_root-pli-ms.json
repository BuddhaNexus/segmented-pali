{
"ja102:1.1": "Jātaka",
"ja102:2.1": "Ekakanipāta",
"ja102:3.1": "Parosatavagga",
"ja102:4.1": "2. Paṇṇikajātaka",
"ja102:5.1": "“Yo dukkhaphuṭṭhāya bhaveyya tāṇaṃ,",
"ja102:5.2": "So me pitā dubbhi vane karoti;",
"ja102:5.3": "Sā kassa kandāmi vanassa majjhe,",
"ja102:5.4": "Yo tāyitā so sahasaṃ karotī”ti.",
"ja102:6.1": "Paṇṇikajātakaṃ dutiyaṃ."
}