{
"ja161:1.1": "Jātaka",
"ja161:2.1": "Dukanipāta",
"ja161:3.1": "Santhavavagga",
"ja161:4.1": "1. Indasamānagottajātaka",
"ja161:5.1": "“Na santhavaṃ kāpurisena kayirā,",
"ja161:5.2": "Ariyo anariyena pajānamatthaṃ;",
"ja161:5.3": "Cirānuvutthopi karoti pāpaṃ,",
"ja161:5.4": "Gajo yathā indasamānagottaṃ.",
"ja161:6.1": "Yaṃ tveva jaññā sadiso mamanti,",
"ja161:6.2": "Sīlena paññāya sutena cāpi;",
"ja161:6.3": "Teneva mettiṃ kayirātha saddhiṃ,",
"ja161:6.4": "Sukho have sappurisena saṅgamo”ti.",
"ja161:7.1": "Indasamānagottajātakaṃ paṭhamaṃ."
}