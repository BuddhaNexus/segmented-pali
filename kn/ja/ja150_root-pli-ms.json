{
"ja150:1.1": "Jātaka",
"ja150:2.1": "Ekakanipāta",
"ja150:3.1": "Kakaṇṭakavagga",
"ja150:4.1": "10. Sañjīvajātaka",
"ja150:5.1": "“Asantaṃ yo paggaṇhāti,",
"ja150:5.2": "asantaṃ cūpasevati;",
"ja150:5.3": "Tameva ghāsaṃ kurute,",
"ja150:5.4": "byaggho sañjīvako yathā”ti.",
"ja150:6.1": "Sañjīvajātakaṃ dasamaṃ.",
"ja150:6.2": "Kakaṇṭakavaggo pannarasamo.",
"ja150:7.1": "Tassuddānaṃ",
"ja150:8.1": "Sukhamedhati daṇḍavaro ca puna,",
"ja150:8.2": "Lasi vāladhi pañcamarādhavaro;",
"ja150:8.3": "Samahodadhi kattika bondi puna,",
"ja150:8.4": "Caturaṅgulabyagghavarena dasāti.",
"ja150:9.1": "Uparimapaṇṇāsako.",
"ja150:10.1": "Atha vagguddānaṃ",
"ja150:11.1": "Apaṇṇakaṃ sīlavaggakuruṅga,",
"ja150:11.2": "Kulāvakaṃ atthakāmena pañcamaṃ;",
"ja150:11.3": "Āsīso itthivaruṇaṃ apāyi,",
"ja150:11.4": "Littavaggena te dasa;",
"ja150:11.5": "Parosataṃ haṃci kusanāḷi,",
"ja150:11.6": "Asampadaṃ kakaṇṭakavaggo.",
"ja150:12.1": "Ekanipātamhilaṅkatanti.",
"ja150:13.1": "Ekakanipātaṃ niṭṭhitaṃ."
}