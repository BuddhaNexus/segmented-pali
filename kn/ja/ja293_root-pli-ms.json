{
"ja293:1.1": "Jātaka",
"ja293:2.1": "Tikanipāta",
"ja293:3.1": "Kumbhavagga",
"ja293:4.1": "3. Kāyanibbindajātaka",
"ja293:5.1": "“Phuṭṭhassa me aññatarena byādhinā,",
"ja293:5.2": "Rogena bāḷhaṃ dukhitassa ruppato;",
"ja293:5.3": "Parisussati khippamidaṃ kaḷevaraṃ,",
"ja293:5.4": "Pupphaṃ yathā paṃsuni ātape kataṃ.",
"ja293:6.1": "Ajaññaṃ jaññasaṅkhātaṃ,",
"ja293:6.2": "asuciṃ sucisammataṃ;",
"ja293:6.3": "Nānākuṇapaparipūraṃ,",
"ja293:6.4": "jaññarūpaṃ apassato.",
"ja293:7.1": "Dhiratthumaṃ āturaṃ pūtikāyaṃ,",
"ja293:7.2": "Jegucchiyaṃ assuciṃ byādhidhammaṃ;",
"ja293:7.3": "Yatthappamattā adhimucchitā pajā,",
"ja293:7.4": "Hāpenti maggaṃ sugatūpapattiyā”ti.",
"ja293:8.1": "Kāyanibbindajātakaṃ tatiyaṃ."
}