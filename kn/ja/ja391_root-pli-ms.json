{
"ja391:1.1": "Jātaka",
"ja391:2.1": "Chakkanipāta",
"ja391:3.1": "Kharaputtavagga",
"ja391:4.1": "6. Vijjādharajātaka",
"ja391:5.1": "“Dubbaṇṇarūpaṃ tuvamariyavaṇṇī,",
"ja391:5.2": "Purakkhatvā pañjaliko namassasi;",
"ja391:5.3": "Seyyo nu te so udavā sarikkho,",
"ja391:5.4": "Nāmaṃ parassattano cāpi brūhi”.",
"ja391:6.1": "“Na nāmagottaṃ gaṇhanti rāja,",
"ja391:6.2": "Sammaggatānujjugatāna devā;",
"ja391:6.3": "Ahañca te nāmadheyyaṃ vadāmi,",
"ja391:6.4": "Sakkohamasmī tidasānamindo”.",
"ja391:7.1": "“Yo disvā bhikkhuṃ caraṇūpapannaṃ,",
"ja391:7.2": "Purakkhatvā pañjaliko namassati;",
"ja391:7.3": "Pucchāmi taṃ devarājetamatthaṃ,",
"ja391:7.4": "Ito cuto kiṃ labhate sukhaṃ so”.",
"ja391:8.1": "“Yo disvā bhikkhuṃ caraṇūpapannaṃ,",
"ja391:8.2": "Purakkhatvā pañjaliko namassati;",
"ja391:8.3": "Diṭṭheva dhamme labhate pasaṃsaṃ,",
"ja391:8.4": "Saggañca so yāti sarīrabhedā”.",
"ja391:9.1": "“Lakkhī vata me udapādi ajja,",
"ja391:9.2": "Yaṃ vāsavaṃ bhūtapatiddasāma;",
"ja391:9.3": "Bhikkhuñca disvāna tuvañca sakka,",
"ja391:9.4": "Kāhāmi puññāni anappakāni”.",
"ja391:10.1": "“Addhā have sevitabbā sapaññā,",
"ja391:10.2": "Bahussutā ye bahuṭhānacintino;",
"ja391:10.3": "Bhikkhuñca disvāna mamañca rāja,",
"ja391:10.4": "Karohi puññāni anappakāni”.",
"ja391:11.1": "“Akkodhano niccapasannacitto,",
"ja391:11.2": "Sabbātithīyācayogo bhavitvā;",
"ja391:11.3": "Nihacca mānaṃ abhivādayissaṃ,",
"ja391:11.4": "Sutvāna devinda subhāsitānī”ti.",
"ja391:12.1": "Vijjādharajātakaṃ chaṭṭhaṃ."
}