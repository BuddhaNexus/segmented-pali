{
"ja228:1.1": "Jātaka",
"ja228:2.1": "Dukanipāta",
"ja228:3.1": "Kāsāvavagga",
"ja228:4.1": "8. Kāmanītajātaka",
"ja228:5.1": "“Tayo giriṃ antaraṃ kāmayāmi,",
"ja228:5.2": "Pañcālā kuruyo kekake ca;",
"ja228:5.3": "Tatuttariṃ brāhmaṇa kāmayāmi,",
"ja228:5.4": "Tikiccha maṃ brāhmaṇa kāmanītaṃ”.",
"ja228:6.1": "“Kaṇhāhidaṭṭhassa karonti heke,",
"ja228:6.2": "Amanussapaviṭṭhassa karonti paṇḍitā;",
"ja228:6.3": "Na kāmanītassa karoti koci,",
"ja228:6.4": "Okkantasukkassa hi kā tikicchā”ti.",
"ja228:7.1": "Kāmanītajātakaṃ aṭṭhamaṃ."
}