{
"ja234:1.1": "Jātaka",
"ja234:2.1": "Dukanipāta",
"ja234:3.1": "Upāhanavagga",
"ja234:4.1": "4. Asitābhūjātaka",
"ja234:5.1": "“Tvameva dānimakara,",
"ja234:5.2": "Yaṃ kāmo byagamā tayi;",
"ja234:5.3": "Soyaṃ appaṭisandhiko,",
"ja234:5.4": "Kharachinnaṃva renukaṃ”.",
"ja234:6.1": "“Atricchaṃ atilobhena,",
"ja234:6.2": "atilobhamadena ca;",
"ja234:6.3": "Evaṃ hāyati atthamhā,",
"ja234:6.4": "ahaṃva asitābhuyā”ti.",
"ja234:7.1": "Asitābhūjātakaṃ catutthaṃ."
}