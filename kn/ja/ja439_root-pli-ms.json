{
"ja439:1.1": "Jātaka",
"ja439:2.1": "Dasakanipāta",
"ja439:3.1": "Catudvāravagga",
"ja439:4.1": "1. Catudvārajātaka",
"ja439:5.1": "“Catudvāramidaṃ nagaraṃ,",
"ja439:5.2": "āyasaṃ daḷhapākāraṃ;",
"ja439:5.3": "Oruddhapaṭiruddhosmi,",
"ja439:5.4": "kiṃ pāpaṃ pakataṃ mayā”.",
"ja439:6.1": "“Sabbe apihitā dvārā,",
"ja439:6.2": "oruddhosmi yathā dijo;",
"ja439:6.3": "Kimādhikaraṇaṃ yakkha,",
"ja439:6.4": "cakkābhinihato ahaṃ.",
"ja439:7.1": "Laddhā satasahassāni,",
"ja439:7.2": "atirekāni vīsati;",
"ja439:7.3": "Anukampakānaṃ ñātīnaṃ,",
"ja439:7.4": "vacanaṃ samma nākari.",
"ja439:8.1": "Laṅghiṃ samuddaṃ pakkhandi,",
"ja439:8.2": "sāgaraṃ appasiddhikaṃ;",
"ja439:8.3": "Catubbhi aṭṭhajjhagamā,",
"ja439:8.4": "aṭṭhāhipi ca soḷasa.",
"ja439:9.1": "Soḷasāhi ca bāttiṃsa,",
"ja439:9.2": "atricchaṃ cakkamāsado;",
"ja439:9.3": "Icchāhatassa posassa,",
"ja439:9.4": "cakkaṃ bhamati matthake.",
"ja439:10.1": "Uparivisālā duppūrā,",
"ja439:10.2": "icchā visaṭagāminī;",
"ja439:10.3": "Ye ca taṃ anugijjhanti,",
"ja439:10.4": "te honti cakkadhārino.",
"ja439:11.1": "Bahubhaṇḍaṃ avahāya,",
"ja439:11.2": "maggaṃ appaṭivekkhiya;",
"ja439:11.3": "Yesañcetaṃ asaṅkhātaṃ,",
"ja439:11.4": "te honti cakkadhārino.",
"ja439:12.1": "Kammaṃ samekkhe vipulañca bhogaṃ,",
"ja439:12.2": "Icchaṃ na seveyya anatthasaṃhitaṃ;",
"ja439:12.3": "Kareyya vākyaṃ anukampakānaṃ,",
"ja439:12.4": "Taṃ tādisaṃ nātivatteyya cakkaṃ”.",
"ja439:13.1": "“Kīvaciraṃ nu me yakkha,",
"ja439:13.2": "cakkaṃ sirasi ṭhassati;",
"ja439:13.3": "Kati vassasahassāni,",
"ja439:13.4": "taṃ me akkhāhi pucchito”.",
"ja439:14.1": "“Atisaro paccasaro,",
"ja439:14.2": "mittavinda suṇohi me;",
"ja439:14.3": "Cakkaṃ te sirasi māviddhaṃ,",
"ja439:14.4": "na taṃ jīvaṃ pamokkhasī”ti.",
"ja439:15.1": "Catudvārajātakaṃ paṭhamaṃ."
}