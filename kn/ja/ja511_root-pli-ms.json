{
"ja511:1.1": "Jātaka",
"ja511:2.1": "Tiṃsanipāta",
"ja511:3.1": "Kiṃchandavagga",
"ja511:4.1": "1. Kiṃchandajātaka",
"ja511:5.1": "“Kiṃchando kimadhippāyo,",
"ja511:5.2": "eko sammasi ghammani;",
"ja511:5.3": "Kiṃ patthayāno kiṃ esaṃ,",
"ja511:5.4": "kena atthena brāhmaṇa”.",
"ja511:6.1": "“Yathā mahā vāridharo,",
"ja511:6.2": "kumbho supariṇāhavā;",
"ja511:6.3": "Tathūpamaṃ ambapakkaṃ,",
"ja511:6.4": "vaṇṇagandharasuttamaṃ.",
"ja511:7.1": "Taṃ vuyhamānaṃ sotena,",
"ja511:7.2": "disvānāmalamajjhime;",
"ja511:7.3": "Pāṇīhi naṃ gahetvāna,",
"ja511:7.4": "agyāyatanamāhariṃ.",
"ja511:8.1": "Tato kadalipattesu,",
"ja511:8.2": "nikkhipitvā sayaṃ ahaṃ;",
"ja511:8.3": "Satthena naṃ vikappetvā,",
"ja511:8.4": "khuppipāsaṃ ahāsi me.",
"ja511:9.1": "Sohaṃ apetadaratho,",
"ja511:9.2": "Byantībhūto dukhakkhamo;",
"ja511:9.3": "Assādaṃ nādhigacchāmi,",
"ja511:9.4": "Phalesvaññesu kesuci.",
"ja511:10.1": "Sosetvā nūna maraṇaṃ,",
"ja511:10.2": "taṃ mamaṃ āvahissati;",
"ja511:10.3": "Ambaṃ yassa phalaṃ sādu,",
"ja511:10.4": "madhuraggaṃ manoramaṃ;",
"ja511:10.5": "Yamuddhariṃ vuyhamānaṃ,",
"ja511:10.6": "udadhismā mahaṇṇave.",
"ja511:11.1": "Akkhātaṃ te mayā sabbaṃ,",
"ja511:11.2": "yasmā upavasāmahaṃ;",
"ja511:11.3": "Rammaṃ pati nisinnosmi,",
"ja511:11.4": "puthulomāyutā puthu.",
"ja511:12.1": "Tvañca kho meva akkhāhi,",
"ja511:12.2": "attānamapalāyini;",
"ja511:12.3": "Kā vā tvamasi kalyāṇi,",
"ja511:12.4": "kissa vā tvaṃ sumajjhime.",
"ja511:13.1": "Ruppapaṭṭapalimaṭṭhīva,",
"ja511:13.2": "byagghīva girisānujā;",
"ja511:13.3": "Yā santi nāriyo devesu,",
"ja511:13.4": "devānaṃ paricārikā.",
"ja511:14.1": "Yā ca manussalokasmiṃ,",
"ja511:14.2": "Rūpenānvāgatitthiyo;",
"ja511:14.3": "Rūpena te sadisī natthi,",
"ja511:14.4": "Devesu gandhabbamanussaloke;",
"ja511:14.5": "Puṭṭhāsi me cārupubbaṅgi,",
"ja511:14.6": "Brūhi nāmañca bandhave”.",
"ja511:15.1": "“Yaṃ tvaṃ pati nisinnosi,",
"ja511:15.2": "rammaṃ brāhmaṇa kosikiṃ;",
"ja511:15.3": "Sāhaṃ bhusālayāvutthā,",
"ja511:15.4": "varavārivahoghasā.",
"ja511:16.1": "Nānādumagaṇākiṇṇā,",
"ja511:16.2": "bahukā girikandarā;",
"ja511:16.3": "Mameva pamukhā honti,",
"ja511:16.4": "abhisandanti pāvuse.",
"ja511:17.1": "Atho bahū vanatodā,",
"ja511:17.2": "nīlavārivahindharā;",
"ja511:17.3": "Bahukā nāgavittodā,",
"ja511:17.4": "abhisandanti vārinā.",
"ja511:18.1": "Tā ambajambulabujā,",
"ja511:18.2": "nīpā tālā cudumbarā;",
"ja511:18.3": "Bahūni phalajātāni,",
"ja511:18.4": "āvahanti abhiṇhaso.",
"ja511:19.1": "Yaṃ kiñci ubhato tīre,",
"ja511:19.2": "phalaṃ patati ambuni;",
"ja511:19.3": "Asaṃsayaṃ taṃ sotassa,",
"ja511:19.4": "phalaṃ hoti vasānugaṃ.",
"ja511:20.1": "Etadaññāya medhāvi,",
"ja511:20.2": "puthupañña suṇohi me;",
"ja511:20.3": "Mā rocaya mabhisaṅgaṃ,",
"ja511:20.4": "paṭisedha janādhipa.",
"ja511:21.1": "Na vāhaṃ vaḍḍhavaṃ maññe,",
"ja511:21.2": "Yaṃ tvaṃ raṭṭhābhivaḍḍhana;",
"ja511:21.3": "Āceyyamāno rājisi,",
"ja511:21.4": "Maraṇaṃ abhikaṅkhasi.",
"ja511:22.1": "Tassa jānanti pitaro,",
"ja511:22.2": "gandhabbā ca sadevakā;",
"ja511:22.3": "Ye cāpi isayo loke,",
"ja511:22.4": "saññatattā tapassino;",
"ja511:22.5": "Asaṃsayaṃ tepi jānanti,",
"ja511:22.6": "paṭṭhabhūtā yasassino”.",
"ja511:23.1": "“Evaṃ viditvā vidū sabbadhammaṃ,",
"ja511:23.2": "Viddhaṃsanaṃ cavanaṃ jīvitassa;",
"ja511:23.3": "Na cīyatī tassa narassa pāpaṃ,",
"ja511:23.4": "Sace na ceteti vadhāya tassa.",
"ja511:24.1": "Isipūgasamaññāte,",
"ja511:24.2": "evaṃ lokyā viditā sati;",
"ja511:24.3": "Anariyaparisambhāse,",
"ja511:24.4": "pāpakammaṃ jigīsasi.",
"ja511:25.1": "Sace ahaṃ marissāmi,",
"ja511:25.2": "tīre te puthusussoṇi;",
"ja511:25.3": "Asaṃsayaṃ taṃ asiloko,",
"ja511:25.4": "mayi pete āgamissati.",
"ja511:26.1": "Tasmā hi pāpakaṃ kammaṃ,",
"ja511:26.2": "rakkhasseva sumajjhime;",
"ja511:26.3": "Mā taṃ sabbo jano pacchā,",
"ja511:26.4": "pakuṭṭhāyi mayi mate”.",
"ja511:27.1": "“Aññātametaṃ avisayhasāhi,",
"ja511:27.2": "Attānamambañca dadāmi te taṃ;",
"ja511:27.3": "So duccaje kāmaguṇe pahāya,",
"ja511:27.4": "Santiñca dhammañca adhiṭṭhitosi.",
"ja511:28.1": "Yo hitvā pubbasaññogaṃ,",
"ja511:28.2": "pacchā saṃyojane ṭhito;",
"ja511:28.3": "Adhammañceva carati,",
"ja511:28.4": "pāpañcassa pavaḍḍhati.",
"ja511:29.1": "Ehi taṃ pāpayissāmi,",
"ja511:29.2": "kāmaṃ appossuko bhava;",
"ja511:29.3": "Upānayāmi sītasmiṃ,",
"ja511:29.4": "viharāhi anussuko.",
"ja511:30.1": "Taṃ puppharasamattebhi,",
"ja511:30.2": "vakkaṅgehi arindama;",
"ja511:30.3": "Koñcā mayūrā diviyā,",
"ja511:30.4": "kolaṭṭhimadhusāḷikā;",
"ja511:30.5": "Kūjitā haṃsapūgehi,",
"ja511:30.6": "kokilettha pabodhare.",
"ja511:31.1": "Ambettha vippasākhaggā,",
"ja511:31.2": "palālakhalasannibhā;",
"ja511:31.3": "Kosambasaḷalā nīpā,",
"ja511:31.4": "pakkatālavilambino”.",
"ja511:32.1": "“Mālī tiriṭī kāyūrī,",
"ja511:32.2": "aṅgadī candanussado;",
"ja511:32.3": "Rattiṃ tvaṃ paricāresi,",
"ja511:32.4": "divā vedesi vedanaṃ.",
"ja511:33.1": "Soḷasitthisahassāni,",
"ja511:33.2": "yā temā paricārikā;",
"ja511:33.3": "Evaṃ mahānubhāvosi,",
"ja511:33.4": "abbhuto lomahaṃsano.",
"ja511:34.1": "Kiṃ kammamakarī pubbe,",
"ja511:34.2": "pāpaṃ attadukhāvahaṃ;",
"ja511:34.3": "Yaṃ karitvā manussesu,",
"ja511:34.4": "piṭṭhimaṃsāni khādasi”.",
"ja511:35.1": "“Ajjhenāni paṭiggayha,",
"ja511:35.2": "kāmesu gadhito ahaṃ;",
"ja511:35.3": "Acariṃ dīghamaddhānaṃ,",
"ja511:35.4": "paresaṃ ahitāyahaṃ.",
"ja511:36.1": "Yo piṭṭhimaṃsiko hoti,",
"ja511:36.2": "evaṃ ukkacca khādati;",
"ja511:36.3": "Yathāhaṃ ajja khādāmi,",
"ja511:36.4": "piṭṭhimaṃsāni attano”ti.",
"ja511:37.1": "Kiṃchandajātakaṃ paṭhamaṃ."
}