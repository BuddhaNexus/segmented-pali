{
"ja222:1.1": "Jātaka",
"ja222:2.1": "Dukanipāta",
"ja222:3.1": "Kāsāvavagga",
"ja222:4.1": "2. Cūḷanandiyajātaka",
"ja222:5.1": "“Idaṃ tadācariyavaco,",
"ja222:5.2": "pārāsariyo yadabravi;",
"ja222:5.3": "Māsu tvaṃ akari pāpaṃ,",
"ja222:5.4": "yaṃ tvaṃ pacchā kataṃ tape.",
"ja222:6.1": "Yāni karoti puriso,",
"ja222:6.2": "tāni attani passati;",
"ja222:6.3": "Kalyāṇakārī kalyāṇaṃ,",
"ja222:6.4": "pāpakārī ca pāpakaṃ;",
"ja222:6.5": "Yādisaṃ vapate bījaṃ,",
"ja222:6.6": "tādisaṃ harate phalan”ti.",
"ja222:7.1": "Cūḷanandiyajātakaṃ dutiyaṃ."
}