{
"ja51:1.1": "Jātaka",
"ja51:2.1": "Ekakanipāta",
"ja51:3.1": "Āsīsavagga",
"ja51:4.1": "1. Mahāsīlavajātaka",
"ja51:5.1": "“Āsīsetheva puriso,",
"ja51:5.2": "na nibbindeyya paṇḍito;",
"ja51:5.3": "Passāmi vohaṃ attānaṃ,",
"ja51:5.4": "yathā icchiṃ tathā ahū”ti.",
"ja51:6.1": "Mahāsīlavajātakaṃ paṭhamaṃ."
}