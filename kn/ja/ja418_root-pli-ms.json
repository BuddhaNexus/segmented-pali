{
"ja418:1.1": "Jātaka",
"ja418:2.1": "Aṭṭhakanipāta",
"ja418:3.1": "Kaccānivagga",
"ja418:4.1": "2. Aṭṭhasaddajātaka",
"ja418:5.1": "“Idaṃ pure ninnamāhu,",
"ja418:5.2": "bahumacchaṃ mahodakaṃ;",
"ja418:5.3": "Āvāso bakarājassa,",
"ja418:5.4": "pettikaṃ bhavanaṃ mama;",
"ja418:5.5": "Tyajja bhekena yāpema,",
"ja418:5.6": "okaṃ na vijahāmase”.",
"ja418:6.1": "“Ko dutiyaṃ asīlissa,",
"ja418:6.2": "bandharassakkhi bhecchati;",
"ja418:6.3": "Ko me putte kulāvakaṃ,",
"ja418:6.4": "mañca sotthiṃ karissati”.",
"ja418:7.1": "“Sabbā parikkhayā pheggu,",
"ja418:7.2": "yāva tassā gatī ahu;",
"ja418:7.3": "Khīṇabhakkho mahārāja,",
"ja418:7.4": "sāre na ramatī ghuṇo”.",
"ja418:8.1": "“Sā nūnāhaṃ ito gantvā,",
"ja418:8.2": "rañño muttā nivesanā;",
"ja418:8.3": "Attānaṃ ramayissāmi,",
"ja418:8.4": "dumasākhaniketinī”.",
"ja418:9.1": "“So nūnāhaṃ ito gantvā,",
"ja418:9.2": "rañño mutto nivesanā;",
"ja418:9.3": "Aggodakāni pissāmi,",
"ja418:9.4": "yūthassa purato vajaṃ”.",
"ja418:10.1": "“Taṃ maṃ kāmehi sammattaṃ,",
"ja418:10.2": "rattaṃ kāmesu mucchitaṃ;",
"ja418:10.3": "Ānayī bharato luddo,",
"ja418:10.4": "bāhiko bhaddamatthu te”.",
"ja418:11.1": "“Andhakāratimisāyaṃ,",
"ja418:11.2": "tuṅge uparipabbate;",
"ja418:11.3": "Sā maṃ saṇhena mudunā,",
"ja418:11.4": "mā pādaṃ khali yasmani”.",
"ja418:12.1": "“Asaṃsayaṃ jātikhayantadassī,",
"ja418:12.2": "Na gabbhaseyyaṃ punarāvajissaṃ;",
"ja418:12.3": "Ayamantimā pacchimā gabbhaseyyā,",
"ja418:12.4": "Khīṇo me saṃsāro punabbhavāyā”ti.",
"ja418:13.1": "Aṭṭhasaddajātakaṃ dutiyaṃ."
}