{
"ja291:1.1": "Jātaka",
"ja291:2.1": "Tikanipāta",
"ja291:3.1": "Kumbhavagga",
"ja291:4.1": "1. Surāghaṭajātaka",
"ja291:5.1": "“Sabbakāmadadaṃ kumbhaṃ,",
"ja291:5.2": "kuṭaṃ laddhāna dhuttako;",
"ja291:5.3": "Yāva naṃ anupāleti,",
"ja291:5.4": "tāva so sukhamedhati.",
"ja291:6.1": "Yadā matto ca ditto ca,",
"ja291:6.2": "pamādā kumbhamabbhidā;",
"ja291:6.3": "Tadā naggo ca pottho ca,",
"ja291:6.4": "pacchā bālo vihaññati.",
"ja291:7.1": "Evameva yo dhanaṃ laddhā,",
"ja291:7.2": "Pamatto paribhuñjati;",
"ja291:7.3": "Pacchā tappati dummedho,",
"ja291:7.4": "Kuṭaṃ bhitvāva dhuttako”ti.",
"ja291:8.1": "Surāghaṭajātakaṃ paṭhamaṃ."
}