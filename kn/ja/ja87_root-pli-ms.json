{
"ja87:1.1": "Jātaka",
"ja87:2.1": "Ekakanipāta",
"ja87:3.1": "Apāyimhavagga",
"ja87:4.1": "7. Maṅgalajātaka",
"ja87:5.1": "“Yassa maṅgalā samūhatāse,",
"ja87:5.2": "Uppātā supinā ca lakkhaṇā ca;",
"ja87:5.3": "So maṅgaladosavītivatto,",
"ja87:5.4": "Yugayogādhigato na jātumetī”ti.",
"ja87:6.1": "Maṅgalajātakaṃ sattamaṃ."
}