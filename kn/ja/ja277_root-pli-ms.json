{
"ja277:1.1": "Jātaka",
"ja277:2.1": "Tikanipāta",
"ja277:3.1": "Udapānavagga",
"ja277:4.1": "7. Romakajātaka",
"ja277:5.1": "“Vassāni paññāsa samādhikāni,",
"ja277:5.2": "Vasimha selassa guhāya romaka;",
"ja277:5.3": "Asaṅkamānā abhinibbutattā,",
"ja277:5.4": "Hatthattamāyanti mamaṇḍajā pure.",
"ja277:6.1": "Te dāni vakkaṅga kimatthamussukā,",
"ja277:6.2": "Bhajanti aññaṃ girikandaraṃ dijā;",
"ja277:6.3": "Na nūna maññanti mamaṃ yathā pure,",
"ja277:6.4": "Cirappavutthā atha vā na te ime”.",
"ja277:7.1": "“Jānāma taṃ na mayaṃ sampamūḷhā,",
"ja277:7.2": "Soyeva tvaṃ te mayamasma nāññe;",
"ja277:7.3": "Cittañca te asmiṃ jane paduṭṭhaṃ,",
"ja277:7.4": "Ājīvikā tena tamuttasāmā”ti.",
"ja277:8.1": "Romakajātakaṃ sattamaṃ."
}