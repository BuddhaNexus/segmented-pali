{
    "ai-ja236:0.1_0": "Jātaka",
    "ai-ja236:0.2_0": "Dukanipāta",
    "ai-ja236:0.3_0": "Upāhanavagga",
    "ai-ja236:0.4_0": "6. Bakajātaka",
    "ai-ja236:1.1_0": "“This one is truly beautiful,",
    "ai-ja236:1.2_0": "of the victor, self-controlled,",
    "ai-ja236:1.3_0": "Rejecting the threat of Virūpakkha,",
    "ai-ja236:1.4_0": "practicing absorption meditation!”",
    "ai-ja236:2.1_0": "“May you give birth to ethics,",
    "ai-ja236:2.2_0": "for they praise",
    "ai-ja236:2.3_0": "Overcome by doing nothing,",
    "ai-ja236:2.4_0": "and no-one gets old.",
    "ai-ja236:3.1_0": "The sixth sub-chapter on arms akimbo."
}