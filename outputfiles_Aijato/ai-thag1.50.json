{
    "thag1.50:0.1_0": "Second:",
    "thag1.50:0.2_0": "One One extinguished One",
    "thag1.50:0.3_0": "Five",
    "thag1.50:0.4_0": "Don’t rub against the wind,",
    "thag1.50:1.1_0": "The rain falls and the wind blows on mother earth,",
    "thag1.50:1.2_0": "while lightning flashes across the sky!",
    "thag1.50:1.3_0": "But my thoughts are stilled,",
    "thag1.50:1.4_0": "my mind is serene.",
    "thag1.50:1.6_0": "a senior monk ...",
    "thag1.50:1.7_0": "the fifth and graceful.",
    "thag1.50:2.0_0": "Inspiring places",
    "thag1.50:2.1_0": "Revata the senior monk Sirivaḍḍha,",
    "thag1.50:2.2_0": "plague bolts with blue net,",
    "thag1.50:2.3_0": "Ramaṇīyavihārin",
    "thag1.50:2.4_0": "is filled with joined palms.",
    "thag1.50:2.5_0": "The senior monk Soṇī",
    "thag1.50:2.6_0": "palace, you prevail over discontent."
}