{
    "thig2.6:0.1_0": "Second: Mallikā",
    "thig2.6:0.2_0": "The Book of the Twos",
    "thig2.6:0.3_0": "Mettiya",
    "thig2.6:1.1_0": "Though in pain,",
    "thig2.6:1.2_0": "feeble, my youth long gone,",
    "thig2.6:1.3_0": "I’ll walk around the staff",
    "thig2.6:1.4_0": "in the mountains.",
    "thig2.6:2.1_0": "Having laid down my outer robe",
    "thig2.6:2.2_0": "where the bowls turn away.",
    "thig2.6:2.3_0": "I sat in the mountain to meditate",
    "thig2.6:2.4_0": "my mind was freed.",
    "thig2.6:2.5_0": "I attained the three knowledges",
    "thig2.6:2.6_0": "having fulfilled the Buddha’s instructions.",
    "thig2.6:3.1_0": "... “The senior monk who had a junior nun.”"
}