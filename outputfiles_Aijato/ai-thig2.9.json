{
    "thig2.9:0.1_0": "Second: Mallikā",
    "thig2.9:0.2_0": "The Book of the Twos",
    "thig2.9:0.3_0": "First: Abhaya",
    "thig2.9:1.1_0": "Abhayā, the body is fragile,",
    "thig2.9:1.2_0": "yet ordinary people are attached to it.",
    "thig2.9:1.3_0": "I'll lay down the body,",
    "thig2.9:1.4_0": "aware and mindful.",
    "thig2.9:2.1_0": "Like a wise person,",
    "thig2.9:2.2_0": "and diligence,",
    "thig2.9:2.3_0": "I have reached the end of craving",
    "thig2.9:2.4_0": "having fulfilled the Buddha’s instructions.",
    "thig2.9:3.1_0": "... there are fearful ..."
}