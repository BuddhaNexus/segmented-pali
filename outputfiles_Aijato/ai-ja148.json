{
    "ai-ja148:0.1_0": "Jātaka",
    "ai-ja148:0.2_0": "Ekakanipāta",
    "ai-ja148:0.3_0": "Kakaṇṭakavagga",
    "ai-ja148:0.4_0": "8. Siṅgālajātaka",
    "ai-ja148:1.1_0": "“I won’t return again,",
    "ai-ja148:1.2_0": "Nor should you leave.",
    "ai-ja148:1.3_0": "I washed my feet",
    "ai-ja148:1.4_0": "and that’s why he’s arguing.”",
    "ai-ja148:2.1_0": "eighth day of the stars."
}