{
    "thag1.30:0.1_0": "Second:",
    "thag1.30:0.2_0": "One One extinguished One",
    "thag1.30:0.3_0": "Third",
    "thag1.30:0.4_0": "this is what they did.",
    "thag1.30:1.1_0": "When I was ill in the past,",
    "thag1.30:1.2_0": "mindfulness arose in me.",
    "thag1.30:1.3_0": "Now I am ill once more—",
    "thag1.30:1.4_0": "it’s time for me to be heedful.",
    "thag1.30:1.6_0": "a senior monk,",
    "thag1.30:1.7_0": "Third: incomplete and impaired.",
    "thag1.30:2.0_0": "Inspiring places",
    "thag1.30:2.1_0": "Nighta, Citta,",
    "thag1.30:2.2_0": "The senior.",
    "thag1.30:2.3_0": "Hearing Nandaka, the senior,",
    "thag1.30:2.4_0": "a senior monk Lomasakaṅgiya,",
    "thag1.30:2.5_0": "Jambugāmikaputta and Sambara,",
    "thag1.30:2.6_0": "and making him a hermit."
}