{
    "ai-tha-ap258:0.1_0": "Therāpadāna",
    "ai-tha-ap258:0.2_0": "Thomakavagga",
    "ai-tha-ap258:0.3_0": "6. Upāhanadāyakattheraapadāna",
    "ai-tha-ap258:1.1_0": "Oh, the moon called Candana,",
    "ai-tha-ap258:1.2_0": "an awakened champion.",
    "ai-tha-ap258:1.3_0": "I’ve given up sensual desire,",
    "ai-tha-ap258:1.4_0": "I am his awakening.",
    "ai-tha-ap258:2.1_0": "At most eons",
    "ai-tha-ap258:2.2_0": "whatever they give,",
    "ai-tha-ap258:2.3_0": "I’m not aware of any bad places",
    "ai-tha-ap258:2.4_0": "The fruit of reap.",
    "ai-tha-ap258:3.1_0": "Attaining the four kinds of textual analysis",
    "ai-tha-ap258:3.2_0": "... “I attained the noble ones,” ...",
    "ai-tha-ap258:3.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap258:4.1_0": "That is how these verses were recited by the senior venerable Kappāyana.",
    "ai-tha-ap258:5.1_0": "sixth part of the senior monks who once was drunk."
}