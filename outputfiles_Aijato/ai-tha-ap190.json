{
    "ai-tha-ap190:0.1_0": "Therāpadāna",
    "ai-tha-ap190:0.2_0": "Kuṭajapupphiyavagga",
    "ai-tha-ap190:0.3_0": "Hearing this coral-tree flowers,",
    "ai-tha-ap190:1.1_0": "“I’m the teacher,",
    "ai-tha-ap190:1.2_0": "honored by the honorable,",
    "ai-tha-ap190:1.3_0": "pus and blood,",
    "ai-tha-ap190:1.4_0": "I recollected the Buddha of the Buddha.",
    "ai-tha-ap190:2.1_0": "A single flower laid down,",
    "ai-tha-ap190:2.2_0": "confidence in his own mind,",
    "ai-tha-ap190:2.3_0": "I stood to one side,",
    "ai-tha-ap190:2.4_0": "seeing clearly,",
    "ai-tha-ap190:3.1_0": "At most eons",
    "ai-tha-ap190:3.2_0": "flowering along with flowers,",
    "ai-tha-ap190:3.3_0": "I’m not aware of any bad places",
    "ai-tha-ap190:3.4_0": "The fruit of the Buddha.",
    "ai-tha-ap190:4.1_0": "I’m seven eons of the world,",
    "ai-tha-ap190:4.2_0": "each into the four oceans.",
    "ai-tha-ap190:4.3_0": "Endowed with the seven gems,",
    "ai-tha-ap190:4.4_0": "the wheel-turning monarch’s bonfire,",
    "ai-tha-ap190:5.1_0": "Attaining the four kinds of textual analysis",
    "ai-tha-ap190:5.2_0": "... “I attained the noble ones,” ...",
    "ai-tha-ap190:5.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap190:6.1_0": "That is how these verses were recited by the senior venerable Bāraṇḍiya.",
    "ai-tha-ap190:7.1_0": "and the eighth day of the flowers."
}