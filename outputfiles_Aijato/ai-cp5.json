{
    "cp5:0.1_0": "Those going to the Kaṇṭaka",
    "cp5:0.2_0": "Patience",
    "cp5:0.3_0": "on giving food,",
    "cp5:0.4_0": "The Great Steward’s teaching,",
    "cp5:1.1_0": "“May the Brahmā realm be fully understood",
    "cp5:1.2_0": "and razor for seven kings.",
    "cp5:1.3_0": "don’t venerate the supreme person,",
    "cp5:1.4_0": "the Great Steward!",
    "cp5:2.1_0": "I fled seven chains",
    "cp5:2.2_0": "that you feed me.",
    "cp5:2.3_0": "I shall give you the great hermit,",
    "cp5:2.4_0": "I’m unsurpassed,",
    "cp5:3.1_0": "May he not give me some wealth, grain,",
    "cp5:3.2_0": "nor do I know them.",
    "cp5:3.3_0": "loved ones,",
    "cp5:3.4_0": "That’s why I speak to you.”",
    "cp5:4.1_0": "the Great Steward, the fifth."
}