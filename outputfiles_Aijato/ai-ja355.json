{
    "ai-ja355:0.1_0": "Jātaka",
    "ai-ja355:0.2_0": "Pañcakanipāta",
    "ai-ja355:0.3_0": "Maṇikuṇḍalavagga",
    "ai-ja355:0.4_0": "5. Ghaṭajātaka",
    "ai-ja355:1.1_0": "“O other people cry to as they cry,",
    "ai-ja355:1.2_0": "of other people.",
    "ai-ja355:1.3_0": "You shine no more—",
    "ai-ja355:1.4_0": "Why don’t you want to touch?”",
    "ai-ja355:2.1_0": "One who is a fool, one who grows,",
    "ai-ja355:2.2_0": "and has come to a year,",
    "ai-ja355:2.3_0": "That’s why I don’t grieve",
    "ai-ja355:2.4_0": "there’s no shelter in going.",
    "ai-ja355:3.1_0": "Because of sorrow, lamentation, pain,",
    "ai-ja355:3.2_0": "don’t prefer tasty food or clothes,",
    "ai-ja355:3.3_0": "their friends are pure,",
    "ai-ja355:3.4_0": "when injured by an arrow strike?",
    "ai-ja355:4.1_0": "by painful mental plans,",
    "ai-ja355:4.2_0": "dispensation it with nothing!",
    "ai-ja355:4.3_0": "Since I’m standing,",
    "ai-ja355:4.4_0": "so I am seen.",
    "ai-ja355:5.1_0": "A mortal of light,",
    "ai-ja355:5.2_0": "of all good people,",
    "ai-ja355:5.3_0": "all the earth,",
    "ai-ja355:5.4_0": "you won’t find happiness.”",
    "ai-ja355:6.1_0": "waterless is the fifth on a bowl,"
}