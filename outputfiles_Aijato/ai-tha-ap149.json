{
    "ai-tha-ap149:0.1_0": "Therāpadāna",
    "ai-tha-ap149:0.2_0": "Chattavagga",
    "ai-tha-ap149:0.3_0": "7. Maggadāyakattheraapadāna",
    "ai-tha-ap149:1.1_0": "I gained the best of men",
    "ai-tha-ap149:1.2_0": "the seer, the Seer!",
    "ai-tha-ap149:1.3_0": "I saw the Buddha,",
    "ai-tha-ap149:1.4_0": "Here are the meaningless verses.",
    "ai-tha-ap149:2.1_0": "trees, free of thorns,",
    "ai-tha-ap149:2.2_0": "having practiced the path,",
    "ai-tha-ap149:2.3_0": "Thinking about the Teacher, the teaching,",
    "ai-tha-ap149:2.4_0": "confidence in his own mind.",
    "ai-tha-ap149:3.1_0": "More than three eons",
    "ai-tha-ap149:3.2_0": "from where deeds I want.",
    "ai-tha-ap149:3.3_0": "I’m not aware of any bad places",
    "ai-tha-ap149:3.4_0": "such a fruit or result.",
    "ai-tha-ap149:4.1_0": "Wisdom",
    "ai-tha-ap149:4.2_0": "I was struck by people.",
    "ai-tha-ap149:4.3_0": "the Sudassana,",
    "ai-tha-ap149:4.4_0": "as a charnel ground.",
    "ai-tha-ap149:5.1_0": "Attaining the four kinds of textual analysis",
    "ai-tha-ap149:5.2_0": "... “I attained the noble ones,” ...",
    "ai-tha-ap149:5.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap149:6.1_0": "That is how these verses were recited by the senior venerable Khujjasobhita.",
    "ai-tha-ap149:7.1_0": "the seventh of the senior ones who goes."
}