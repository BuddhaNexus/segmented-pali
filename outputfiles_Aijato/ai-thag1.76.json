{
    "ai-thag1.76:0.1_0": "Verses of the Senior Monks",
    "ai-thag1.76:0.2_0": "The Book of the Ones",
    "ai-thag1.76:0.3_0": "Chapter Eight",
    "ai-thag1.76:0.4_0": "1.76. Piyañjaha",
    "ai-thag1.76:1.1_0": "Settle down when others spring up;",
    "ai-thag1.76:1.2_0": "spring up when others settle down.",
    "ai-thag1.76:1.3_0": "Remain when others have departed;",
    "ai-thag1.76:1.4_0": "don’t delight when others delight.",
    "ai-thag1.76:1.5_0": "... “The senior monk with lovely eyes!”"
}