{
    "thag1.23:0.1_0": "Second:",
    "thag1.23:0.2_0": "One One extinguished One",
    "thag1.23:0.3_0": "Third",
    "thag1.23:0.4_0": "this sal tree trunk,",
    "thag1.23:1.1_0": "I’ll eat honey and milk-rice",
    "thag1.23:1.2_0": "in Veḷugumba.",
    "thag1.23:1.3_0": "And then, skillfully scrutinizing",
    "thag1.23:1.4_0": "of the rise and fall of the aggregates,",
    "thag1.23:1.5_0": "I’ll return to my forest hill",
    "thag1.23:1.6_0": "and foster seclusion.",
    "thag1.23:1.7_0": "Gosāla,"
}