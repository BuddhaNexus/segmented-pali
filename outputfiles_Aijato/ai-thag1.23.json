{
    "ai-thag1.23:0.1_0": "Verses of the Senior Monks",
    "ai-thag1.23:0.2_0": "The Book of the Ones",
    "ai-thag1.23:0.3_0": "Chapter Three",
    "ai-thag1.23:0.4_0": "1.23. Gosāla",
    "ai-thag1.23:1.1_0": "I’ll eat honey and milk-rice",
    "ai-thag1.23:1.2_0": "in Veḷugumba.",
    "ai-thag1.23:1.3_0": "And then, skillfully scrutinizing",
    "ai-thag1.23:1.4_0": "of the rise and fall of the aggregates,",
    "ai-thag1.23:1.5_0": "I’ll return to my forest hill",
    "ai-thag1.23:1.6_0": "and foster seclusion.",
    "ai-thag1.23:1.7_0": "Gosāla,"
}