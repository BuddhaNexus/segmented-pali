{
    "ai-ja2:0.1_0": "Jātaka",
    "ai-ja2:0.2_0": "Ekakanipāta",
    "ai-ja2:0.3_0": "Apaṇṇakavagga",
    "ai-ja2:0.4_0": "2. Vaṇṇupathajātaka",
    "ai-ja2:1.1_0": "“One who alms-blacks",
    "ai-ja2:1.2_0": "springs up there again and again,",
    "ai-ja2:1.3_0": "When the noble ones are still alive,",
    "ai-ja2:1.4_0": "but your heart is unfettered.”",
    "ai-ja2:2.1_0": "Second: beauty and glory."
}