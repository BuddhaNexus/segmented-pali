{
    "ai-ja153:0.1_0": "Jātaka",
    "ai-ja153:0.2_0": "Dukanipāta",
    "ai-ja153:0.3_0": "Daḷhavagga",
    "ai-ja153:0.4_0": "3. Sūkarajātaka",
    "ai-ja153:1.1_0": "“My dear Pañcasikha,",
    "ai-ja153:1.2_0": "Even the four supports,",
    "ai-ja153:1.3_0": "Come back, my mother,",
    "ai-ja153:1.4_0": "What’s the point of scared?”",
    "ai-ja153:2.1_0": "You suffered, stinking,",
    "ai-ja153:2.2_0": "A stinking, a pig’s, or a fish.",
    "ai-ja153:2.3_0": "If you want to listen to the teaching,",
    "ai-ja153:2.4_0": "I give you this simile.”",
    "ai-ja153:3.1_0": "third from Bālakalogs."
}