{
    "ai-kv23.2:0.1_0": "Kathāvatthu",
    "ai-kv23.2:0.2_0": "Mahāpaṇṇāsaka",
    "ai-kv23.2:0.3_0": "Tevīsatimavagga",
    "ai-kv23.2:0.4_0": "Arahantavaṇṇakathā",
    "ai-kv23.2:1.1_0": "having sexual intercourse with a perfected one.",
    "ai-kv23.2:1.2_0": "and the summoned them.",
    "ai-kv23.2:1.3_0": "killed living creatures ...",
    "ai-kv23.2:1.4_0": "Those who steal:",
    "ai-kv23.2:1.5_0": "speak falsely,",
    "ai-kv23.2:1.6_0": "speak divisively,",
    "ai-kv23.2:1.7_0": "harsh words speak with harsh words.",
    "ai-kv23.2:1.8_0": "they’ve taken up,",
    "ai-kv23.2:1.9_0": "They cut off the cliff,",
    "ai-kv23.2:1.10_0": "They cut the creeper",
    "ai-kv23.2:1.11_0": "Sitting alone, sleeping alone,",
    "ai-kv23.2:1.12_0": "Like a lady",
    "ai-kv23.2:1.13_0": "commit adultery,",
    "ai-kv23.2:1.14_0": "killed. ...",
    "ai-kv23.2:1.15_0": "along with raids.",
    "ai-kv23.2:1.16_0": "For there are no decrepitments.",
    "ai-kv23.2:1.17_0": "The discussion of the perfected ones is finished."
