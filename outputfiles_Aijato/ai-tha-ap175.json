{
    "ai-tha-ap175:0.1_0": "Therāpadāna",
    "ai-tha-ap175:0.2_0": "Kumudavagga",
    "ai-tha-ap175:0.3_0": "3 Rattipupphiyattheraapadāna",
    "ai-tha-ap175:1.1_0": "I used to be in a sorry state.",
    "ai-tha-ap175:1.2_0": "in the wilderness,",
    "ai-tha-ap175:1.3_0": "I saw the Buddha",
    "ai-tha-ap175:1.4_0": "a god surrounded by gods.",
    "ai-tha-ap175:2.1_0": "seeing the flowers,",
    "ai-tha-ap175:2.2_0": "with your receptiles.",
    "ai-tha-ap175:2.3_0": "they’re rooted and all,",
    "ai-tha-ap175:2.4_0": "the great hermits.",
    "ai-tha-ap175:3.1_0": "At most eons",
    "ai-tha-ap175:3.2_0": "flowering on a flower",
    "ai-tha-ap175:3.3_0": "I’m not aware of any bad places",
    "ai-tha-ap175:3.4_0": "flowers, fruit, and fruit.",
    "ai-tha-ap175:4.1_0": "here there are eight eons",
    "ai-tha-ap175:4.2_0": "Welcome, good-hearted,",
    "ai-tha-ap175:4.3_0": "Endowed with the seven gems,",
    "ai-tha-ap175:4.4_0": "Mahaka is queen",
    "ai-tha-ap175:5.1_0": "Attaining the four kinds of textual analysis",
    "ai-tha-ap175:5.2_0": "... “I attained the noble ones,” ...",
    "ai-tha-ap175:5.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap175:6.1_0": "That is how these verses were recited by the senior venerable Bakkula.",
    "ai-tha-ap175:7.1_0": "and the third flowers of the senior monks."
}