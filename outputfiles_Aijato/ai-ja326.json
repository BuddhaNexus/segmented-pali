{
    "ai-ja326:0.1_0": "Jātaka",
    "ai-ja326:0.2_0": "Catukkanipāta",
    "ai-ja326:0.3_0": "Kuṭidūsakavagga",
    "ai-ja326:0.4_0": "6. Kakkārujātaka",
    "ai-ja326:1.1_0": "I’d better have no use",
    "ai-ja326:1.2_0": "speak falsely,",
    "ai-ja326:1.3_0": "But by gaining fame,",
    "ai-ja326:1.4_0": "that whipools.”",
    "ai-ja326:2.1_0": "“We will live together—",
    "ai-ja326:2.2_0": "nor take wealth.",
    "ai-ja326:2.3_0": "They don’t get material possessions,",
    "ai-ja326:2.4_0": "that whipools.”",
    "ai-ja326:3.1_0": "“When a mendicant’s mind is born,",
    "ai-ja326:3.2_0": "faithful and kind,",
    "ai-ja326:3.3_0": "After eating, all that’s considered,",
    "ai-ja326:3.4_0": "that whipools.”",
    "ai-ja326:4.1_0": "“Whether in the world or the world beyond,",
    "ai-ja326:4.2_0": "If there’s no kindness,",
    "ai-ja326:4.3_0": "As he spoke, so he acted;",
    "ai-ja326:4.4_0": "that whipools.”",
    "ai-ja326:5.1_0": "The sixth sub-chapter on mirroring."
}