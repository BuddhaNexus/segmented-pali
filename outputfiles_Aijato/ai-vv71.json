{
    "vv71:0.1_0": "Thoughts",
    "vv71:0.2_0": "Mango Grove",
    "vv71:0.3_0": "With Pāyāsi",
    "vv71:0.4_0": "On one occasion the keepers of the Licchavīs, poetry-workers saw.",
    "vv71:1.1_0": "“This gem is worthless",
    "vv71:1.2_0": "... “I attained the noble ones,” ...",
    "vv71:1.3_0": "your beauty shines like the quarter.”",
    "vv71:2.1_0": "The god was delighted and happy,",
    "vv71:2.2_0": "... “I attained the noble ones,” ...",
    "vv71:2.3_0": "that is the fruit of deeds.",
    "vv71:3.1_0": "I’m a human being,",
    "vv71:3.2_0": "I was ascended a keeper.",
    "vv71:3.3_0": "I saw the stainless monk",
    "vv71:3.4_0": "clear and undisturbed.”",
    "vv71:4.1_0": "I’m afraid of this,",
    "vv71:4.2_0": "confidence in living creatures,",
    "vv71:4.3_0": "Giving up material things,",
    "vv71:4.4_0": "I delight in the forest grove.",
    "vv71:5.1_0": "With my appearance of me,",
    "vv71:5.2_0": "... “I attained the noble ones,” ...",
    "vv71:5.3_0": "My self-wokes like the sky.",
    "vv71:6.1_0": "you’ll take the seventh palace."
}