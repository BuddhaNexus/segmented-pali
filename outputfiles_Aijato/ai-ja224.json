{
    "ai-ja224:0.1_0": "Jātaka",
    "ai-ja224:0.2_0": "Dukanipāta",
    "ai-ja224:0.3_0": "Kāsāvavagga",
    "ai-ja224:0.4_0": "4. Kumbhilajātaka",
    "ai-ja224:1.1_0": "“A faithful householder",
    "ai-ja224:1.2_0": "as other, not as self,",
    "ai-ja224:1.3_0": "does not grieve after passing away:",
    "ai-ja224:1.4_0": "seen with the seen.",
    "ai-ja224:2.1_0": "Who makes no mistakes,",
    "ai-ja224:2.2_0": "he is a most of the compassionate,",
    "ai-ja224:2.3_0": "does not grieve after passing away:",
    "ai-ja224:2.4_0": "that’s what you’ve seen.”",
    "ai-ja224:3.1_0": "on the fourth pot."
}