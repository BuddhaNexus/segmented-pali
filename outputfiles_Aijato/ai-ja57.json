{
    "ai-ja57:0.1_0": "Jātaka",
    "ai-ja57:0.2_0": "Ekakanipāta",
    "ai-ja57:0.3_0": "Āsīsavagga",
    "ai-ja57:0.4_0": "7. Vānarindajātaka",
    "ai-ja57:1.1_0": "“A faithful householder",
    "ai-ja57:1.2_0": "as if they are;",
    "ai-ja57:1.3_0": "does not grieve after passing away:",
    "ai-ja57:1.4_0": "that’s seen what you’ve seen.”",
    "ai-ja57:2.1_0": "on the seventh day, the seventhter."
}