{
    "thig2.10:0.1_0": "Second: Mallikā",
    "thig2.10:0.2_0": "The Book of the Twos",
    "thig2.10:0.3_0": "Sāmāvatī,",
    "thig2.10:1.1_0": "Three times",
    "thig2.10:1.2_0": "I left my dwelling;",
    "thig2.10:1.3_0": "Failing to find peace of heart,",
    "thig2.10:1.4_0": "lacking control over my mind.",
    "thig2.10:1.5_0": "Now it is the eighth night",
    "thig2.10:1.6_0": "since craving was eradicated.",
    "thig2.10:2.1_0": "Like a wise person,",
    "thig2.10:2.2_0": "and diligence,",
    "thig2.10:2.3_0": "I have reached the end of craving",
    "thig2.10:2.4_0": "having fulfilled the Buddha’s instructions.",
    "thig2.10:3.2_0": "the senior monk,",
    "thig2.10:4.1_0": "The Book of the Twos is finished."
}