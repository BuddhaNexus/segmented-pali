{
    "ai-tha-ap168:0.1_0": "Therāpadāna",
    "ai-tha-ap168:0.2_0": "Supāricariyavagga",
    "ai-tha-ap168:0.3_0": "6. Sappidāyakattheraapadāna",
    "ai-tha-ap168:1.1_0": "“I wasn’t word about the Buddha,",
    "ai-tha-ap168:1.2_0": "receiver of sacrifices,",
    "ai-tha-ap168:1.3_0": "Please come back where this hero,",
    "ai-tha-ap168:1.4_0": "the great light cleft of fire.",
    "ai-tha-ap168:2.1_0": "The Blessed One gradually,",
    "ai-tha-ap168:2.2_0": "I come to my presence,",
    "ai-tha-ap168:2.3_0": "From there, having attained a bowl,",
    "ai-tha-ap168:2.4_0": "like ghee.",
    "ai-tha-ap168:3.1_0": "All the eons",
    "ai-tha-ap168:3.2_0": "ghee.",
    "ai-tha-ap168:3.3_0": "I’m not aware of any bad places",
    "ai-tha-ap168:3.4_0": "The fruit of ghee.",
    "ai-tha-ap168:4.1_0": "Wisdom",
    "ai-tha-ap168:4.2_0": "alone, companion,",
    "ai-tha-ap168:4.3_0": "Endowed with the seven gems,",
    "ai-tha-ap168:4.4_0": "Mahāpajāpatī Gotamī.",
    "ai-tha-ap168:5.1_0": "Attaining the four kinds of textual analysis",
    "ai-tha-ap168:5.2_0": "... “I attained the noble ones,” ...",
    "ai-tha-ap168:5.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap168:6.1_0": "That is how these verses were recited by the senior venerable ghee.",
    "ai-tha-ap168:7.1_0": "The sixth part of ghee."
}