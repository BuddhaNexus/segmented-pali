{
    "ai-thag1.20:0.1_0": "Verses of the Senior Monks",
    "ai-thag1.20:0.2_0": "The Book of the Ones",
    "ai-thag1.20:0.3_0": "Chapter Two",
    "ai-thag1.20:0.4_0": "1.20. Ajita",
    "ai-thag1.20:1.1_0": "I do not fear death;",
    "ai-thag1.20:1.2_0": "nor do I long for life.",
    "ai-thag1.20:1.3_0": "I’ll lay down this body,",
    "ai-thag1.20:1.4_0": "aware and mindful.",
    "ai-thag1.20:1.5_0": "…",
    "ai-thag1.20:1.6_0": "Ajita ...",
    "ai-thag1.20:1.7_0": "Second: your second",
    "ai-thag1.20:2.0_0": "Inspiring places",
    "ai-thag1.20:2.1_0": "like a great wind,",
    "ai-thag1.20:2.2_0": "like a snake smeared with clouds.",
    "ai-thag1.20:2.3_0": "Belaṭṭhasīsa,",
    "ai-thag1.20:2.4_0": "and a slave and servant.",
    "ai-thag1.20:2.5_0": "the senior monk who Sikhī,",
    "ai-thag1.20:2.6_0": "and there’s the ten."
}