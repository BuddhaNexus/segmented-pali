{
    "ai-ja61:0.1_0": "Jātaka",
    "ai-ja61:0.2_0": "Ekakanipāta",
    "ai-ja61:0.3_0": "Itthivagga",
    "ai-ja61:0.4_0": "1. Asātamantajātaka",
    "ai-ja61:1.1_0": "“A woman with a man,",
    "ai-ja61:1.2_0": "nor are they found at all.",
    "ai-ja61:1.3_0": "But if anyone who comes to heartwood,",
    "ai-ja61:1.4_0": "like a daughter-in-law,",
    "ai-ja61:1.5_0": "we shall go forth,",
    "ai-ja61:1.6_0": "and foster seclusion.",
    "ai-ja61:2.1_0": "first from the mind,"
}