{
    "thag2.10:0.1_0": "Second:",
    "thag2.10:0.2_0": "The Book of the Twos",
    "thag2.10:0.3_0": "First",
    "thag2.10:0.4_0": "this is the mark:",
    "thag2.10:1.1_0": "First one kills oneself,",
    "thag2.10:1.2_0": "then one kills others.",
    "thag2.10:1.3_0": "One kills oneself, really dead,",
    "thag2.10:1.4_0": "like one who kills birds using a dead bird as a decoy.",
    "thag2.10:2.1_0": "A holy man’s color is not on the outside;",
    "thag2.10:2.2_0": "a holy man is colored on the inside.",
    "thag2.10:2.3_0": "Whoever harbors bad deeds",
    "thag2.10:2.4_0": "is truly a dark one, Sujampati.",
    "thag2.10:2.6_0": "the senior monk,",
    "thag2.10:2.7_0": "first and grabbed.",
    "thag2.10:3.0_0": "Inspiring places",
    "thag2.10:3.1_0": "And Uttara and Piṇḍola,",
    "thag2.10:3.2_0": "and Aṭṭhaka, Vāmaka,",
    "thag2.10:3.3_0": "unselfish, born of love,",
    "thag2.10:3.4_0": "clean up,",
    "thag2.10:3.5_0": "These six stays together—",
    "thag2.10:3.6_0": "Ten supernormal powers."
}