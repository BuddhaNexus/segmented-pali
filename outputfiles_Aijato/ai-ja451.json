{
    "ai-ja451:0.1_0": "Jātaka",
    "ai-ja451:0.2_0": "Dasakanipāta",
    "ai-ja451:0.3_0": "Catudvāravagga",
    "ai-ja451:0.4_0": "13. Cakkavākajātaka",
    "ai-ja451:1.1_0": "He was beautiful and graceful,",
    "ai-ja451:1.2_0": "flowering for houses,",
    "ai-ja451:1.3_0": "a liar with good eyesight,",
    "ai-ja451:1.4_0": "with clear and undisturbed faculties.",
    "ai-ja451:2.1_0": "the city of Pāvā,",
    "ai-ja451:2.2_0": "a strength of rewards,",
    "ai-ja451:2.3_0": "sitting on the Ganges riverbank",
    "ai-ja451:2.4_0": "so I enjoy my food.”",
    "ai-ja451:3.1_0": "“I eat this food,",
    "ai-ja451:3.2_0": "clothes or hair,",
    "ai-ja451:3.3_0": "Except if they associate with,",
    "ai-ja451:3.4_0": "I’ve designated that food.”",
    "ai-ja451:4.1_0": "“I have no sound,",
    "ai-ja451:4.2_0": "wheel is excellent,",
    "ai-ja451:4.3_0": "I too eat,",
    "ai-ja451:4.4_0": "and always philoughed.",
    "ai-ja451:5.1_0": "Among a meal,",
    "ai-ja451:5.2_0": "with pure meat sauce I love,",
    "ai-ja451:5.3_0": "You wouldn’t have such power,",
    "ai-ja451:5.4_0": "like a wheel, the ox.”",
    "ai-ja451:6.1_0": "“Living in self,",
    "ai-ja451:6.2_0": "beast as human wealth,",
    "ai-ja451:6.3_0": "Beggars progress,",
    "ai-ja451:6.4_0": "and he taught his Dhamma.",
    "ai-ja451:7.1_0": "he has escaped the whole world,",
    "ai-ja451:7.2_0": "with wicked deeds.",
    "ai-ja451:7.3_0": "He’s unlinkable,",
    "ai-ja451:7.4_0": "and he taught his Dhamma.",
    "ai-ja451:8.1_0": "I too eat,",
    "ai-ja451:8.2_0": "snake, sloth, and all creatures.",
    "ai-ja451:8.3_0": "at ease, free of hope,",
    "ai-ja451:8.4_0": "sorrowless, fearful, sorrowless.",
    "ai-ja451:9.1_0": "When they’ve developed their physical endurance,",
    "ai-ja451:9.2_0": "transcending what precepts,",
    "ai-ja451:9.3_0": "harmlessness, delight,",
    "ai-ja451:9.4_0": "I’m dear to me.",
    "ai-ja451:10.1_0": "They don’t kill living creatures,",
    "ai-ja451:10.2_0": "that conqueror cannot conquer the victor,",
    "ai-ja451:10.3_0": "with love for all sentient beings,",
    "ai-ja451:10.4_0": "they have no enmity for anyone.”",
    "ai-ja451:11.1_0": "wheel, curse, and yoke."
}