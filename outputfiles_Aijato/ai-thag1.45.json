{
    "ai-thag1.45:0.1_0": "Verses of the Senior Monks",
    "ai-thag1.45:0.2_0": "The Book of the Ones",
    "ai-thag1.45:0.3_0": "Chapter Five",
    "ai-thag1.45:0.4_0": "this is the dwellings that are pleasant,",
    "ai-thag1.45:1.1_0": "Just as a fine thoroughbred",
    "ai-thag1.45:1.2_0": "stands on dry land,",
    "ai-thag1.45:1.3_0": "Even so, an astute person accomplished in vision,",
    "ai-thag1.45:1.4_0": "a disciple of the Buddha.",
    "ai-thag1.45:1.5_0": "... who live diligently."
}