{
    "ai-thag2.6:0.1_0": "Verses of the Senior Monks",
    "ai-thag2.6:0.2_0": "The Book of the Twos",
    "ai-thag2.6:0.3_0": "Chapter One",
    "ai-thag2.6:0.4_0": "2.6. Meḷajina",
    "ai-thag2.6:1.1_0": "When I heard the Teacher",
    "ai-thag2.6:1.2_0": "speaking Dhamma,",
    "ai-thag2.6:1.3_0": "I wasn’t aware of any doubt",
    "ai-thag2.6:1.4_0": "because of all the kings.",
