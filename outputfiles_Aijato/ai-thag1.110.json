{
    "ai-thag1.110:0.1_0": "Verses of the Senior Monks",
    "ai-thag1.110:0.2_0": "The Book of the Ones",
    "ai-thag1.110:0.3_0": "Chapter Eleven",
    "ai-thag1.110:0.4_0": "1.110. Usabha (1st)",
    "ai-thag1.110:1.1_0": "The trees on the mountain-tops have grown tall,",
    "ai-thag1.110:1.2_0": "freshly sprinkled by towering clouds.",
    "ai-thag1.110:1.3_0": "For Usabha, who loves seclusion, and who thinks only of wilderness,",
    "ai-thag1.110:1.4_0": "goodness flourishes more and more.",
    "ai-thag1.110:1.5_0": "…",
    "ai-thag1.110:1.6_0": "a senior monk,",
    "ai-thag1.110:1.7_0": "eleventh.",
    "ai-thag1.110:2.0_0": "Inspiring places",
    "ai-thag1.110:2.1_0": "The turbans cries",
    "ai-thag1.110:2.2_0": "Brahmadatta,",
    "ai-thag1.110:2.3_0": "My arms used to be so pretty,",
    "ai-thag1.110:2.4_0": "firm in principle, and moral,",
    "ai-thag1.110:2.5_0": "Saṅgharakkhita and senior,",
    "ai-thag1.110:2.6_0": "a great sage and a sage."
}