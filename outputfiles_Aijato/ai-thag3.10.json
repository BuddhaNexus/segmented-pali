{
    "thag3.10:0.1_0": "Second:",
    "thag3.10:0.2_0": "The Book of the Threes",
    "thag3.10:0.3_0": "First",
    "thag3.10:0.4_0": "this is the judge",
    "thag3.10:1.1_0": "In the past you had faith,",
    "thag3.10:1.2_0": "today you have none.",
    "thag3.10:1.3_0": "They know you",
