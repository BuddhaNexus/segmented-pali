{
    "ai-ja232:0.1_0": "Jātaka",
    "ai-ja232:0.2_0": "Dukanipāta",
    "ai-ja232:0.3_0": "Upāhanavagga",
    "ai-ja232:0.4_0": "2. Vīṇāguṇajātaka",
    "ai-ja232:1.1_0": "“Child is the purpose of Yama,",
    "ai-ja232:1.2_0": "the fool is unsullied.",
    "ai-ja232:1.3_0": "But by hard to strive,",
    "ai-ja232:1.4_0": "you should stay together.”",
    "ai-ja232:2.1_0": "“Even if I am a man, he perceives both",
    "ai-ja232:2.2_0": "I am laughing,",
    "ai-ja232:2.3_0": "this bag of bones,",
    "ai-ja232:2.4_0": "like a bird snare?”",
    "ai-ja232:3.1_0": "Second: multi-layered soles with flowers."
}