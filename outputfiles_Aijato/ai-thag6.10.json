{
    "ai-thag6.10:0.1_0": "Verses of the Senior Monks",
    "ai-thag6.10:0.2_0": "The Book of the Sixes",
    "ai-thag6.10:0.3_0": "Chapter One",
    "ai-thag6.10:0.4_0": "6.10. Sumana (2nd)",
    "ai-thag6.10:1.1_0": "I was only seven years old",
    "ai-thag6.10:1.2_0": "and had just gone forth",
    "ai-thag6.10:1.3_0": "when I overcame the mighty serpent king",
    "ai-thag6.10:1.4_0": "with my psychic powers.",
    "ai-thag6.10:2.1_0": "I brought water for my mentor",
    "ai-thag6.10:2.2_0": "from the great lake Anotatta.",
    "ai-thag6.10:2.3_0": "When he saw me,",
    "ai-thag6.10:2.4_0": "my teacher declared:",
    "ai-thag6.10:3.1_0": "“Sāriputta, see this",
    "ai-thag6.10:3.2_0": "young boy coming,",
    "ai-thag6.10:3.3_0": "carrying a water pot,",
    "ai-thag6.10:3.4_0": "serene inside himself.",
    "ai-thag6.10:4.1_0": "His conduct inspires confidence,",
    "ai-thag6.10:4.2_0": "he is of lovely deportment:",
    "ai-thag6.10:4.3_0": "he is Anuruddha’s novice,",
    "ai-thag6.10:4.4_0": "venerated by psychic power and dance.",
    "ai-thag6.10:5.1_0": "Made a thoroughbred by a thoroughbred,",
    "ai-thag6.10:5.2_0": "made good by the good,",
    "ai-thag6.10:5.3_0": "educated and trained by Anuruddha,",
    "ai-thag6.10:5.4_0": "her task completed, resolute.",
    "ai-thag6.10:6.1_0": "Having attained ultimate peace,",
    "ai-thag6.10:6.2_0": "and witnessed the unshakable,",
    "ai-thag6.10:6.3_0": "that novice Sumana has the wish:",
    "ai-thag6.10:6.4_0": "‘May no-one find me out!’”",
    "ai-thag6.10:6.5_0": "a senior monk ..."
}