{
    "ai-thag1.70:0.1_0": "Verses of the Senior Monks",
    "ai-thag1.70:0.2_0": "The Book of the Ones",
    "ai-thag1.70:0.3_0": "Chapter Seven",
    "ai-thag1.70:0.4_0": "1.70. Puṇṇa (2nd)",
    "ai-thag1.70:1.1_0": "Ethical conduct is best in this life,",
    "ai-thag1.70:1.2_0": "wise and virtuous,",
    "ai-thag1.70:1.3_0": "Among gods and humans,",
    "ai-thag1.70:1.4_0": "ethics, immersion, and wisdom.”",
    "ai-thag1.70:1.5_0": "…",
    "ai-thag1.70:1.6_0": "a senior monk ...",
    "ai-thag1.70:1.7_0": "and he was impaired.",
    "ai-thag1.70:2.0_0": "Inspiring places",
    "ai-thag1.70:2.1_0": "Vajjiputta and Pajjiputta,",
    "ai-thag1.70:2.2_0": "Vimalakoṇḍañña,",
    "ai-thag1.70:2.3_0": "But when it comes to a little,",
    "ai-thag1.70:2.4_0": "Meghiya the one thing,",
    "ai-thag1.70:2.5_0": "and not frequenting fodderations,",
    "ai-thag1.70:2.6_0": "Mahaka.”"
}