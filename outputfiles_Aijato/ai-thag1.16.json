{
    "thag1.16:0.1_0": "Second:",
    "thag1.16:0.2_0": "One One extinguished One",
    "thag1.16:0.3_0": "Second",
    "thag1.16:0.4_0": "Standing on the bank of the river Sappinī,",
    "thag1.16:1.1_0": "Just as a fine thoroughbred",
    "thag1.16:1.2_0": "proceeds with ease,",
    "thag1.16:1.3_0": "tail and mane flying in the wind;",
    "thag1.16:1.4_0": "so my days and nights",
    "thag1.16:1.5_0": "proceed with ease,",
    "thag1.16:1.6_0": "full of spiritual joy.",
    "thag1.16:1.7_0": "Belaṭṭhasīsa,"
}