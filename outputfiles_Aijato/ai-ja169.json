{
    "ai-ja169:0.1_0": "Jātaka",
    "ai-ja169:0.2_0": "Dukanipāta",
    "ai-ja169:0.3_0": "Santhavavagga",
    "ai-ja169:0.4_0": "9. Arakajātaka",
    "ai-ja169:1.1_0": "“When someone with a mind of love,",
    "ai-ja169:1.2_0": "and having compassion for the whole world,",
    "ai-ja169:1.3_0": "below, across,",
    "ai-ja169:1.4_0": "with limitless heart.",
    "ai-ja169:2.1_0": "Your mind is limitless,",
    "ai-ja169:2.2_0": "and gradually consolidated",
    "ai-ja169:2.3_0": "any deed that emerges",
    "ai-ja169:2.4_0": "He’ll remain silent.”",
    "ai-ja169:3.1_0": "ninety."
}