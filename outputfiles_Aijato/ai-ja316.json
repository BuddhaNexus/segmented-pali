{
    "ai-ja316:0.1_0": "Jātaka",
    "ai-ja316:0.2_0": "Catukkanipāta",
    "ai-ja316:0.3_0": "Pucimandavagga",
    "ai-ja316:0.4_0": "6. Sasapaṇḍitajātaka",
    "ai-ja316:1.1_0": "My friend,",
    "ai-ja316:1.2_0": "from the water to the shore.",
    "ai-ja316:1.3_0": "I have a brahmin,",
    "ai-ja316:1.4_0": "this is the dreadful racket.”",
    "ai-ja316:2.1_0": "My provisions is the field",
    "ai-ja316:2.2_0": "or night for food,",
    "ai-ja316:2.3_0": "Sensual pleasures, two on medium,",
    "ai-ja316:2.4_0": "Other curd",
    "ai-ja316:2.5_0": "I have a brahmin,",
    "ai-ja316:2.6_0": "this is the dreadful racket.”",
    "ai-ja316:3.1_0": "“For the cold, the cold,",
    "ai-ja316:3.2_0": "cooled and delightful;",
    "ai-ja316:3.3_0": "I have a brahmin,",
    "ai-ja316:3.4_0": "this is the dreadful racket.”",
    "ai-ja316:4.1_0": "Because of a lake there’s no shelter,",
    "ai-ja316:4.2_0": "a naked goblin,",
    "ai-ja316:4.3_0": "This fire,",
    "ai-ja316:4.4_0": "which I stayed in the forest grove.”",
    "ai-ja316:5.1_0": "to the sixth hidden."
}