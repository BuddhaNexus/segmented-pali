{
    "ai-ja246:0.1_0": "Jātaka",
    "ai-ja246:0.2_0": "Dukanipāta",
    "ai-ja246:0.3_0": "Siṅgālavagga",
    "ai-ja246:0.4_0": "6. Bālovādajātaka",
    "ai-ja246:1.1_0": "Having cut off killing,",
    "ai-ja246:1.2_0": "One gives a gift to an untrue teaching.",
    "ai-ja246:1.3_0": "Alms, eating,",
    "ai-ja246:1.4_0": "with wickedness.",
    "ai-ja246:2.1_0": "“Knowing a person’s mother,",
    "ai-ja246:2.2_0": "One gives a gift to an untrue teaching.",
    "ai-ja246:2.3_0": "Made one with food,",
    "ai-ja246:2.4_0": "but not by wickedness.",
    "ai-ja246:3.1_0": "The sixth section on the fool."
}