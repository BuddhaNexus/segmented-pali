{
    "ai-ja308:0.1_0": "Jātaka",
    "ai-ja308:0.2_0": "Catukkanipāta",
    "ai-ja308:0.3_0": "Kāliṅgavagga",
    "ai-ja308:0.4_0": "8. Sakuṇajātaka",
    "ai-ja308:1.1_0": "“The duty of one whose business is happy;",
    "ai-ja308:1.2_0": "curbed the power of sacrifice.",
    "ai-ja308:1.3_0": "Meditate like a deer-hunter,",
    "ai-ja308:1.4_0": "to get anything bad.”",
    "ai-ja308:2.1_0": "My nose was ashes,",
    "ai-ja308:2.2_0": "always by greedy,",
    "ai-ja308:2.3_0": "Tamed, peaceful,",
    "ai-ja308:2.4_0": "to live on this living.”",
    "ai-ja308:3.1_0": "“My lady whose questions have been granted!",
    "ai-ja308:3.2_0": "So much without Soṇa,",
    "ai-ja308:3.3_0": "“While still a trainee, there’s nothing left over,",
    "ai-ja308:3.4_0": "associate with you.",
    "ai-ja308:4.1_0": "Kuvera in the world",
    "ai-ja308:4.2_0": "and bad friends,",
    "ai-ja308:4.3_0": "listening to the true teaching",
    "ai-ja308:4.4_0": "and left that place.”",
    "ai-ja308:5.1_0": "and the eighth birds."
}