{
    "ai-ja323:0.1_0": "Jātaka",
    "ai-ja323:0.2_0": "Catukkanipāta",
    "ai-ja323:0.3_0": "Kuṭidūsakavagga",
    "ai-ja323:0.4_0": "3. Brahmadattajātaka",
    "ai-ja323:1.1_0": "I am the only one,",
    "ai-ja323:1.2_0": "or Brahmadatta.",
    "ai-ja323:1.3_0": "loss, wealth, or health;",
    "ai-ja323:1.4_0": "Thoughts are just the same.",
    "ai-ja323:2.1_0": "The purgatives crying:",
    "ai-ja323:2.2_0": "the five hundred chariots of the chariots.",
    "ai-ja323:2.3_0": "Whoever has renounced me",
    "ai-ja323:2.4_0": "from darkness.",
    "ai-ja323:3.1_0": "Don’t see me, but crying:",
    "ai-ja323:3.2_0": "and ten that is easy to find.",
    "ai-ja323:3.3_0": "They’re profounding with you,",
    "ai-ja323:3.4_0": "I wish to sit in private alone.”",
    "ai-ja323:4.1_0": "“I am a brahmin, brahmin,",
    "ai-ja323:4.2_0": "with a thousand caves,",
    "ai-ja323:4.3_0": "For how does noble one not tremble?",
    "ai-ja323:4.4_0": "I shall discard a thousand evils.”",
    "ai-ja323:5.1_0": "and the third Brahmadatta."
}