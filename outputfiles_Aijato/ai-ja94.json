{
    "ai-ja94:0.1_0": "Jātaka",
    "ai-ja94:0.2_0": "Ekakanipāta",
    "ai-ja94:0.3_0": "Littavagga",
    "ai-ja94:0.4_0": "4. Lomahaṁsajātaka",
    "ai-ja94:1.1_0": "that wise one,",
    "ai-ja94:1.2_0": "alone in the awe-inspiring forest.",
    "ai-ja94:1.3_0": "Naked, no fire to sit beside,",
    "ai-ja94:1.4_0": "the sage still pursues his quest.”",
    "ai-ja94:2.1_0": "whose fourth subsided is finished."
}