{
    "ai-thag1.40:0.1_0": "Verses of the Senior Monks",
    "ai-thag1.40:0.2_0": "The Book of the Ones",
    "ai-thag1.40:0.3_0": "Chapter Four",
    "ai-thag1.40:0.4_0": "1.40. Vaḍḍhamāna",
    "ai-thag1.40:1.1_0": "“Like a mighty thing,",
    "ai-thag1.40:1.2_0": "rounded on the head.",
    "ai-thag1.40:1.3_0": "But this giving up desire for rebirth,",
    "ai-thag1.40:1.4_0": "with few possessions, not full of desire.",
    "ai-thag1.40:1.5_0": "…",
    "ai-thag1.40:1.6_0": "and the senior monk grows.",
    "ai-thag1.40:1.7_0": "Fourth: graceful and thoughtful.",
    "ai-thag1.40:2.0_0": "Inspiring places",
    "ai-thag1.40:2.1_0": "Though there are crocodiles,",
    "ai-thag1.40:2.2_0": "all the fracked things.",
    "ai-thag1.40:2.3_0": "But the ascetic life of the ascetic life,",
    "ai-thag1.40:2.4_0": "the son of the Sakyans,",
    "ai-thag1.40:2.5_0": "Gavampati is the senior",
    "ai-thag1.40:2.6_0": "the great mass of water."
}