{
    "ai-pv47:0.1_0": "Petavatthu",
    "ai-pv47:0.2_0": "Mahāvagga",
    "ai-pv47:0.3_0": "12. Ambavanapetavatthu",
    "ai-pv47:1.1_0": "“This lotus pond is lovely,",
    "ai-pv47:1.2_0": "and the mother-in-law,",
    "ai-pv47:1.3_0": "be as sweet as a breeze,",
    "ai-pv47:1.4_0": "How do you get permission from the mind?",
    "ai-pv47:2.1_0": "You’ve enjoyed the mango grove,",
    "ai-pv47:2.2_0": "all the fruits of knowledge.",
    "ai-pv47:2.3_0": "Remember the flower",
    "ai-pv47:2.4_0": "How do you get angry?”",
    "ai-pv47:3.1_0": "“Many rice porridge is the result of past deeds,",
    "ai-pv47:3.2_0": "cooled and delightful;",
    "ai-pv47:3.3_0": "Woe to give to the gift",
    "ai-pv47:3.4_0": "That’s where I dislike.”",
    "ai-pv47:4.1_0": "“Rejoiced over this life,",
    "ai-pv47:4.2_0": "giving, self-control, and restraint;",
    "ai-pv47:4.3_0": "I’ll have broken rice,",
    "ai-pv47:4.4_0": "I went to the hermitage of the hermits.”",
    "ai-pv47:5.1_0": "twelfth for the mangoes."
}