{
    "ai-ja383:0.1_0": "Jātaka",
    "ai-ja383:0.2_0": "Chakkanipāta",
    "ai-ja383:0.3_0": "Avāriyavagga",
    "ai-ja383:0.4_0": "8. Kukkuṭajātaka",
    "ai-ja383:1.1_0": "“Like a tall forest grove",
    "ai-ja383:1.2_0": "gone to the far shore,",
    "ai-ja383:1.3_0": "But of the feeble,",
    "ai-ja383:1.4_0": "my wife I’m free from head.”",
    "ai-ja383:2.1_0": "“Happy, lady,",
    "ai-ja383:2.2_0": "two delightful things long",
    "ai-ja383:2.3_0": "Like a deer roasting,",
    "ai-ja383:2.4_0": "seeks another husband.”",
    "ai-ja383:3.1_0": "“I shall divide you,”",
    "ai-ja383:3.2_0": "adorned with jewellery;",
    "ai-ja383:3.3_0": "Not seeing me by noble wisdom,",
    "ai-ja383:3.4_0": "She’ll borrow.",
    "ai-ja383:4.1_0": "“Pakudhaka, are reducive, and blood,",
    "ai-ja383:4.2_0": "bandits and chickens,",
    "ai-ja383:4.3_0": "you don’t tremble with noble wisdom,",
    "ai-ja383:4.4_0": "I want my husband.”",
    "ai-ja383:5.1_0": "“Someone who carries out a mirage,",
    "ai-ja383:5.2_0": "seeing them growing old,",
    "ai-ja383:5.3_0": "Introducing each other with gentle words,",
    "ai-ja383:5.4_0": "as a chicken with its pigs.",
    "ai-ja383:6.1_0": "An astute person understands",
    "ai-ja383:6.2_0": "they don’t achieve what they have.",
    "ai-ja383:6.3_0": "Like a mighty bridge,",
    "ai-ja383:6.4_0": "after then they later.",
    "ai-ja383:7.1_0": "An astute person understands",
    "ai-ja383:7.2_0": "that leads to extinguishment.",
    "ai-ja383:7.3_0": "Together with the lay life,",
    "ai-ja383:7.4_0": "like a chicken pig.”",
    "ai-ja383:8.1_0": "Eight jackals."
}