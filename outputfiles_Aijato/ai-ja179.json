{
    "ai-ja179:0.1_0": "Jātaka",
    "ai-ja179:0.2_0": "Dukanipāta",
    "ai-ja179:0.3_0": "Kalyāṇavagga",
    "ai-ja179:0.4_0": "9. Satadhammajātaka",
    "ai-ja179:1.1_0": "“Childrents,",
    "ai-ja179:1.2_0": "with a sense of conscience, not with difficulty.",
    "ai-ja179:1.3_0": "I’ve left behind the brahmin,",
    "ai-ja179:1.4_0": "and whatever is lost.”",
    "ai-ja179:2.1_0": "He taught this Dhamma,",
    "ai-ja179:2.2_0": "anyone who loves life,",
    "ai-ja179:2.3_0": "being honored by the good,",
    "ai-ja179:2.4_0": "even if he does not delight.",
    "ai-ja179:3.1_0": "The ninth section on the teaching of the seven celebration."
}