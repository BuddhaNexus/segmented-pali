{
    "tha-ap262:0.1_0": "The senior monks, Householders,",
    "tha-ap262:0.2_0": "Honored",
    "tha-ap262:0.3_0": "Hermits to the most senior monk flowers,",
    "tha-ap262:1.1_0": "Awakened the color of others,",
    "tha-ap262:1.2_0": "in the mountains;",
    "tha-ap262:1.3_0": "Shining with light,",
    "tha-ap262:1.4_0": "to see her as happening.",
    "tha-ap262:10.1_0": "It’s a bridge,",
    "tha-ap262:10.2_0": "Campā was the seventh wondering;",
    "tha-ap262:10.3_0": "searing the flower",
    "tha-ap262:10.4_0": "this hut made of flowers.",
    "tha-ap262:10.5_0": "seems to me now,",
    "tha-ap262:10.6_0": "Either one root it leads the sapling.",
    "tha-ap262:2.1_0": "Flame Tree, flowers,",
    "tha-ap262:2.2_0": "I used to go to the supreme place.",
    "tha-ap262:2.3_0": "confident and settled,",
    "tha-ap262:2.4_0": "in the Buddha’s presence.",
    "tha-ap262:3.1_0": "The lie down on the same sheet",
    "tha-ap262:3.2_0": "flowering along with flowers,",
    "tha-ap262:3.3_0": "I’m not aware of any bad places",
    "tha-ap262:3.4_0": "The fruit of the Buddha.",
    "tha-ap262:4.1_0": "Twenty-five years have passed,",
    "tha-ap262:4.2_0": "I became depressed.",
    "tha-ap262:4.3_0": "Endowed with the seven gems,",
    "tha-ap262:4.4_0": "Mahāpajāpatī Gotamī.",
    "tha-ap262:5.1_0": "Attaining the four kinds of textual analysis",
    "tha-ap262:5.2_0": "... “I attained the noble ones,” ...",
    "tha-ap262:5.3_0": "and fulfilled the Buddha’s instructions.”",
    "tha-ap262:6.1_0": "That is how these verses were recited by the senior venerable Bodhi.",
    "tha-ap262:7.1_0": "The tenth flowers, the senior.",
    "tha-ap262:8.1_0": "The ground is finished.",
    "tha-ap262:9.1_0": "Inspiring places"
}