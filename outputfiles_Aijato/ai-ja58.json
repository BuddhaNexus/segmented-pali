{
    "ai-ja58:0.1_0": "Jātaka",
    "ai-ja58:0.2_0": "Ekakanipāta",
    "ai-ja58:0.3_0": "Āsīsavagga",
    "ai-ja58:0.4_0": "8. Tayodhammajātaka",
    "ai-ja58:1.1_0": "“Whoever has these three things",
    "ai-ja58:1.2_0": "as other, not as self,",
    "ai-ja58:1.3_0": "of the sun, wisdom,",
    "ai-ja58:1.4_0": "that’s seen what you’ve seen.”",
    "ai-ja58:2.1_0": "Three on the eighth day."
}