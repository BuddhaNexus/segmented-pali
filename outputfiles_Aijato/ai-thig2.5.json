{
    "ai-thig2.5:0.1_0": "Verses of the Senior Nuns",
    "ai-thig2.5:0.2_0": "The Book of the Twos",
    "ai-thig2.5:0.3_0": "2.5. Cittā",
    "ai-thig2.5:1.1_0": "Though I’m skinny,",
    "ai-thig2.5:1.2_0": "sick, and very feeble,",
    "ai-thig2.5:1.3_0": "I’ll walk right there",
    "ai-thig2.5:1.4_0": "in the mountains.",
    "ai-thig2.5:2.1_0": "Having laid down my outer robe,",
    "ai-thig2.5:2.2_0": "where the bowls turn away.",
    "ai-thig2.5:2.3_0": "propping myself against a rock,",
    "ai-thig2.5:2.4_0": "I shattered the mass of darkness.",
    "ai-thig2.5:3.1_0": "... the senior monk ..."
}