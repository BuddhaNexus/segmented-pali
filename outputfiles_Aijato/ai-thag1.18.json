{
    "thag1.18:0.1_0": "Second:",
    "thag1.18:0.2_0": "One One extinguished One",
    "thag1.18:0.3_0": "Second",
    "thag1.18:0.4_0": "grew sidebly,",
    "thag1.18:1.1_0": "There was an heir of the Buddha,",
    "thag1.18:1.2_0": "a monk in Bhesakaḷā forest,",
    "thag1.18:1.3_0": "who suffused the entire earth",
    "thag1.18:1.4_0": "with the perception of bones.",
    "thag1.18:1.5_0": "I think he will quickly",
    "thag1.18:1.6_0": "get rid of sensual desire.",
    "thag1.18:1.7_0": "... who are senior monk of Yuna."
}