{
    "thag1.49:0.1_0": "Second:",
    "thag1.49:0.2_0": "One One extinguished One",
    "thag1.49:0.3_0": "Five",
    "thag1.49:0.4_0": "and enjoyed himself with householders.",
    "thag1.49:1.1_0": "Even with all the sounds,",
    "thag1.49:1.2_0": "the chirping and cheeping of the birds,",
    "thag1.49:1.3_0": "my mind doesn’t waver,",
    "thag1.49:1.4_0": "for I’m devoted to oneness.",
    "thag1.49:1.5_0": "Rāmaṇeyyaka,"
}