{
    "ai-thag2.33:0.1_0": "Verses of the Senior Monks",
    "ai-thag2.33:0.2_0": "The Book of the Twos",
    "ai-thag2.33:0.3_0": "Chapter Four",
    "ai-thag2.33:0.4_0": "2.33. Upavāṇa",
    "ai-thag2.33:1.1_0": "The perfected one, the Holy One in the world,",
    "ai-thag2.33:1.2_0": "a sage recovered from the good.",
    "ai-thag2.33:1.3_0": "If there is hot water,",
    "ai-thag2.33:1.4_0": "taught me the Dhamma.",
    "ai-thag2.33:2.1_0": "honored by the honorable,",
    "ai-thag2.33:2.2_0": "the honored by the honorable,",
    "ai-thag2.33:2.3_0": "honored by the honorable,",
    "ai-thag2.33:2.4_0": "venerated by the venerable.”",
    "ai-thag2.33:2.5_0": "... the senior monk Upavāṇa of the senior monks ..."
}