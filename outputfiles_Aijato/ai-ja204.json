{
    "ai-ja204:0.1_0": "Jātaka",
    "ai-ja204:0.2_0": "Dukanipāta",
    "ai-ja204:0.3_0": "Nataṁdaḷhavagga",
    "ai-ja204:0.4_0": "4. Vīrakajātaka",
    "ai-ja204:1.1_0": "I saw my hero:",
    "ai-ja204:1.2_0": "like a bird blowing in a charnel ground.",
    "ai-ja204:1.3_0": "We’ve gone forth in hopes of eating,",
    "ai-ja204:1.4_0": "with my answer.”",
    "ai-ja204:2.1_0": "Compassionate for the Virūpakkhas,",
    "ai-ja204:2.2_0": "always stingy,",
    "ai-ja204:2.3_0": "Remember the threat of patches,",
    "ai-ja204:2.4_0": "frequented by a pretext, wearing a cloak.”",
    "ai-ja204:3.1_0": "your hero is finished."
}