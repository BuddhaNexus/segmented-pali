{
    "ai-vv14:0.1_0": "Vimānavatthu",
    "ai-vv14:0.2_0": "Itthivimāna",
    "ai-vv14:0.3_0": "Pīṭhavagga",
    "ai-vv14:0.4_0": "14. Dutiyasuṇisāvimānavatthu",
    "ai-vv14:1.1_0": "“Any man by overexerting,",
    "ai-vv14:1.2_0": "that you stand firm, sir,",
    "ai-vv14:1.3_0": "sunk in every direction",
    "ai-vv14:1.4_0": "and ozing for such a one.",
    "ai-vv14:2.1_0": "Why did you praise them?",
    "ai-vv14:2.2_0": "so where would you attain?",
    "ai-vv14:2.3_0": "But since they’re wealthy,",
    "ai-vv14:2.4_0": "any kind of hymns.",
    "ai-vv14:3.1_0": "I ask you, mighty, powerful,",
    "ai-vv14:3.2_0": "You’re doing good deeds by human beings!",
    "ai-vv14:3.3_0": "By their hair, wrap?",
    "ai-vv14:3.4_0": "your beauty shines like the quarter.”",
    "ai-vv14:4.1_0": "Delighted by the deities,",
    "ai-vv14:4.2_0": "Moggallāna asked Moggallāna",
    "ai-vv14:4.3_0": "answered questions;",
    "ai-vv14:4.4_0": "that is the fruit of deeds.",
    "ai-vv14:5.1_0": "I am human beings,",
    "ai-vv14:5.2_0": "I listened to the house of a daughter-in-law.",
    "ai-vv14:6.1_0": "I saw the stainless monk",
    "ai-vv14:6.2_0": "clear and undisturbed,",
    "ai-vv14:6.3_0": "I’m afraid of this,",
    "ai-vv14:6.4_0": "with confidence in those without it,",
    "ai-vv14:6.5_0": "Giving up material things,",
    "ai-vv14:6.6_0": "I delight in the forest grove.",
    "ai-vv14:7.1_0": "With my appearance of love,",
    "ai-vv14:7.2_0": "And it gains me here,",
    "ai-vv14:7.3_0": "But when I’ve come to my wealth,",
    "ai-vv14:7.4_0": "any kind of hymns.",
    "ai-vv14:8.1_0": "I’m said to you,",
    "ai-vv14:8.2_0": "having made merit for human beings.",
    "ai-vv14:8.3_0": "so too, trapped in this way,",
    "ai-vv14:8.4_0": "My self-wokes like the sky.",
    "ai-vv14:9.1_0": "Second: it had fourteen."
}