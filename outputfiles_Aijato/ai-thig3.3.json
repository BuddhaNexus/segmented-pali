{
    "ai-thig3.3:0.1_0": "Verses of the Senior Nuns",
    "ai-thig3.3:0.2_0": "The Book of the Threes",
    "ai-thig3.3:0.3_0": "3.3. Another Uttamā",
    "ai-thig3.3:1.1_0": "“These seven awakening factors,",
    "ai-thig3.3:1.2_0": "for realizing the path,",
    "ai-thig3.3:1.3_0": "I’ve developed all these things.",
    "ai-thig3.3:1.4_0": "as it was taught by the Buddha.",
    "ai-thig3.3:2.1_0": "I attain the meditations on emptiness",
    "ai-thig3.3:2.2_0": "and signlessness whenever I want.",
    "ai-thig3.3:2.3_0": "I am the Buddha’s rightful daughter,",
    "ai-thig3.3:2.4_0": "always delighting in quenching.",
    "ai-thig3.3:3.1_0": "All sensual pleasures are cut off,",
    "ai-thig3.3:3.2_0": "whether human or divine.",
    "ai-thig3.3:3.3_0": "transmigration through births is finished:",
    "ai-thig3.3:3.4_0": "Now there is no further existence.”",
    "ai-thig3.3:4.1_0": "the most senior monk ..."
}