{
    "ai-ja216:0.1_0": "Jātaka",
    "ai-ja216:0.2_0": "Dukanipāta",
    "ai-ja216:0.3_0": "Bīraṇathambhavagga",
    "ai-ja216:0.4_0": "6. Macchajātaka",
    "ai-ja216:1.1_0": "“Purity comes the path,",
    "ai-ja216:1.2_0": "a branch of pigs,",
    "ai-ja216:1.3_0": "attack me,",
    "ai-ja216:1.4_0": "discontent, and desire—",
    "ai-ja216:2.1_0": "From the fire of greed burns me,",
    "ai-ja216:2.2_0": "The mind leads to peace.",
    "ai-ja216:2.3_0": "free of thorns,",
    "ai-ja216:2.4_0": "doesn’t fall under the sway of sensual pleasures.”",
    "ai-ja216:3.1_0": "sixth fish."
}