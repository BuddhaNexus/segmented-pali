{
    "ai-tha-ap208:0.1_0": "Therāpadāna",
    "ai-tha-ap208:0.2_0": "Kaṇikārapupphiyavagga",
    "ai-tha-ap208:0.3_0": "6. Udakadāyakattheraapadāna",
    "ai-tha-ap208:1.1_0": "Since he saw the ascetic,",
    "ai-tha-ap208:1.2_0": "clear and undisturbed,",
    "ai-tha-ap208:1.3_0": "Like a boat of water,",
    "ai-tha-ap208:1.4_0": "of the birds in the sky.",
    "ai-tha-ap208:2.1_0": "I’m stainless,",
    "ai-tha-ap208:2.2_0": "cheerless and humble,",
    "ai-tha-ap208:2.3_0": "When continued existence has become extinguished,",
    "ai-tha-ap208:2.4_0": "I’ve realized the fruit.",
    "ai-tha-ap208:3.1_0": "More than three eons",
    "ai-tha-ap208:3.2_0": "and water,",
    "ai-tha-ap208:3.3_0": "I’m not aware of any bad places",
    "ai-tha-ap208:3.4_0": "The fruit of its result.",
    "ai-tha-ap208:4.1_0": "Many eons",
    "ai-tha-ap208:4.2_0": "And so he immaculate himself.",
    "ai-tha-ap208:4.3_0": "Endowed with the seven gems,",
    "ai-tha-ap208:4.4_0": "Mahāpajāpatī Gotamī.",
    "ai-tha-ap208:5.1_0": "Attaining the four kinds of textual analysis",
    "ai-tha-ap208:5.2_0": "... “I attained the noble ones,” ...",
    "ai-tha-ap208:5.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap208:6.1_0": "That is how these verses were recited by the senior venerable senior venerable Dīgha Tapassī.",
    "ai-tha-ap208:7.1_0": "sixth senior monk who is a sixth maker;"
}