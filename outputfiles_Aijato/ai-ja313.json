{
    "ja313:0.1_0": "The group of twelve on “as expected”",
    "ja313:0.2_0": "The Book of the Fours",
    "ja313:0.3_0": "At Māghasa",
    "ja313:0.4_0": "Don’t speak harshly,",
    "ja313:1.1_0": "“Whoever gets your hands and feet,",
    "ja313:1.2_0": "and my ears and nose,",
    "ja313:1.3_0": "Tortoise the great hero,",
    "ja313:1.4_0": "those within the country.”",
    "ja313:2.1_0": "“Attacking my hands and feet,",
    "ja313:2.2_0": "and my ears and nose,",
    "ja313:2.3_0": "So long as a king live,",
    "ja313:2.4_0": "they don’t found the quarters.",
    "ja313:3.1_0": "with the past and future,",
    "ja313:3.2_0": "an ascetic is ruined.",
    "ja313:3.3_0": "Still, steady, and standing,",
    "ja313:3.4_0": "and he’s unable to hold a fanner.",
    "ja313:4.1_0": "By these words,",
    "ja313:4.2_0": "resulting in a catch.",
    "ja313:4.3_0": "The king of Kāsī,",
    "ja313:4.4_0": "you’ll grieve when sent to hell.",
    "ja313:5.1_0": "for one who speaks in accordance with the Teaching;"
}