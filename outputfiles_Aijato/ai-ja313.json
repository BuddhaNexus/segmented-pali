{
    "ai-ja313:0.1_0": "Jātaka",
    "ai-ja313:0.2_0": "Catukkanipāta",
    "ai-ja313:0.3_0": "Pucimandavagga",
    "ai-ja313:0.4_0": "3. Khantīvādījātaka",
    "ai-ja313:1.1_0": "“Whoever gets your hands and feet,",
    "ai-ja313:1.2_0": "and my ears and nose,",
    "ai-ja313:1.3_0": "Tortoise the great hero,",
    "ai-ja313:1.4_0": "those within the country.”",
    "ai-ja313:2.1_0": "“Attacking my hands and feet,",
    "ai-ja313:2.2_0": "and my ears and nose,",
    "ai-ja313:2.3_0": "So long as a king live,",
    "ai-ja313:2.4_0": "they don’t found the quarters.",
    "ai-ja313:3.1_0": "with the past and future,",
    "ai-ja313:3.2_0": "an ascetic is ruined.",
    "ai-ja313:3.3_0": "Still, steady, and standing,",
    "ai-ja313:3.4_0": "and he’s unable to hold a fanner.",
    "ai-ja313:4.1_0": "By these words,",
    "ai-ja313:4.2_0": "resulting in a catch.",
    "ai-ja313:4.3_0": "The king of Kāsī,",
    "ai-ja313:4.4_0": "you’ll grieve when sent to hell.",
    "ai-ja313:5.1_0": "for one who speaks in accordance with the Teaching;"
}