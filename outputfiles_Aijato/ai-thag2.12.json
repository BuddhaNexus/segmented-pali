{
    "ai-thag2.12:0.1_0": "Verses of the Senior Monks",
    "ai-thag2.12:0.2_0": "The Book of the Twos",
    "ai-thag2.12:0.3_0": "Chapter Two",
    "ai-thag2.12:0.4_0": "2.12. Jotidāsa",
    "ai-thag2.12:1.1_0": "People who act harshly—",
    "ai-thag2.12:1.2_0": "attacking people,",
    "ai-thag2.12:1.3_0": "tying them up,",
    "ai-thag2.12:1.4_0": "hurting them in all kinds of ways—",
    "ai-thag2.12:1.5_0": "they’re treated in the same way;",
    "ai-thag2.12:1.6_0": "their deeds don’t vanish.",
    "ai-thag2.12:2.1_0": "Whatever deeds a person does,",
    "ai-thag2.12:2.2_0": "whether good or bad,",
    "ai-thag2.12:2.3_0": "I’ll be sovereign of them,",
    "ai-thag2.12:2.4_0": "and every deed they do.",
    "ai-thag2.12:2.5_0": "as a senior monk ..."
}