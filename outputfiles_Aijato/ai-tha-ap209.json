{
    "ai-tha-ap209:0.1_0": "Therāpadāna",
    "ai-tha-ap209:0.2_0": "Kaṇikārapupphiyavagga",
    "ai-tha-ap209:0.3_0": "7 Salalamāliyattheraapadāna",
    "ai-tha-ap209:1.1_0": "“How many folk are practicing absorption,",
    "ai-tha-ap209:1.2_0": "in the mountains;",
    "ai-tha-ap209:1.3_0": "sunk in every direction",
    "ai-tha-ap209:1.4_0": "Hermits to Ship",
    "ai-tha-ap209:2.1_0": "Having completed the wealth,",
    "ai-tha-ap209:2.2_0": "I was running out and circling around.",
    "ai-tha-ap209:2.3_0": "Covered with flowers,",
    "ai-tha-ap209:2.4_0": "in the Buddha’s presence.",
    "ai-tha-ap209:3.1_0": "More than three eons",
    "ai-tha-ap209:3.2_0": "flowering on a flower",
    "ai-tha-ap209:3.3_0": "I’m not aware of any bad places",
    "ai-tha-ap209:3.4_0": "The fruit of the Buddha.",
    "ai-tha-ap209:4.1_0": "Having been Wisdom",
    "ai-tha-ap209:4.2_0": "I alone light, O light.",
    "ai-tha-ap209:4.3_0": "Endowed with the seven gems,",
    "ai-tha-ap209:4.4_0": "Mahāpajāpatī Gotamī.",
    "ai-tha-ap209:5.1_0": "Attaining the four kinds of textual analysis",
    "ai-tha-ap209:5.2_0": "... “I attained the noble ones,” ...",
    "ai-tha-ap209:5.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap209:6.1_0": "That is how these verses were recited by the senior venerable Makhādeva Mango Grove.",
    "ai-tha-ap209:7.1_0": "The seventh quarrels of the senior ones."
}