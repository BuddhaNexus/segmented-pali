{
    "ai-tha-ap362:0.1_0": "Therāpadāna",
    "ai-tha-ap362:0.2_0": "Saddasaññakavagga",
    "ai-tha-ap362:0.3_0": "10. Piṇḍapātikattheraapadāna",
    "ai-tha-ap362:1.1_0": "“The Awakened as a Buddha,",
    "ai-tha-ap362:1.2_0": "I burned with discernment.",
    "ai-tha-ap362:1.3_0": "Here you gathered here,",
    "ai-tha-ap362:1.4_0": "with alms-food.",
    "ai-tha-ap362:2.1_0": "Swept away by the Buddha,",
    "ai-tha-ap362:2.2_0": "and there is a great lake named Mahākappina.",
    "ai-tha-ap362:2.3_0": "confidence in their heart,",
    "ai-tha-ap362:2.4_0": "I’ve gone to an end.",
    "ai-tha-ap362:3.1_0": "All the eons",
    "ai-tha-ap362:3.2_0": "seeing the giver of giving.",
    "ai-tha-ap362:3.3_0": "I’m not aware of any bad places",
    "ai-tha-ap362:3.4_0": "alms-food is fruitless.",
    "ai-tha-ap362:4.1_0": "Attaining the four kinds of textual analysis",
    "ai-tha-ap362:4.2_0": "... “I attained the noble ones,” ...",
    "ai-tha-ap362:4.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap362:5.1_0": "That is how these verses were recited by the senior venerable Kāsi.",
    "ai-tha-ap362:6.1_0": "He is the tenth part of the senior monks.",
    "ai-tha-ap362:7.1_0": "He is included in the charnel ground.",
    "ai-tha-ap362:8.1_0": "Inspiring places",
    "ai-tha-ap362:9.1_0": "Living in a charnel ground,",
    "ai-tha-ap362:9.2_0": "What’s flower?",
    "ai-tha-ap362:9.3_0": "Ambank to mango grove",
    "ai-tha-ap362:9.4_0": "a bed with angular legs,",
    "ai-tha-ap362:9.5_0": "Go for refuge",
    "ai-tha-ap362:9.6_0": "until they reached verses."
}