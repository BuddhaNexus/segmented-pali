{
    "ai-snp5.15:0.1_0": "Sutta Nipāta 5.15",
    "ai-snp5.15:0.2_0": "Posālamāṇavapucchā",
    "ai-snp5.15:1.1_0": "“A man who reveals the past,",
    "ai-snp5.15:1.2_0": "said Venerable Pañcasikha,",
    "ai-snp5.15:1.3_0": "unstirred, their doubts cut off,",
    "ai-snp5.15:1.4_0": "They’ve gone beyond all things,",
    "ai-snp5.15:1.5_0": "I left behind.",
    "ai-snp5.15:2.1_0": "for the voice of limits,",
    "ai-snp5.15:2.2_0": "who has given up everything,",
    "ai-snp5.15:2.3_0": "whether pleasure or pain",
    "ai-snp5.15:2.4_0": "who knows and sees,",
    "ai-snp5.15:2.5_0": "I ask you the knowledge",
    "ai-snp5.15:2.6_0": "What kind of person does he say?”",
    "ai-snp5.15:3.1_0": "All-seeing knowledges,",
    "ai-snp5.15:3.2_0": "said the Buddha to Sela,",
    "ai-snp5.15:3.3_0": "the Realized One, the Buddha,",
    "ai-snp5.15:3.4_0": "if the seat is leftover and",
    "ai-snp5.15:3.5_0": "who is freed with the Tapodā.",
    "ai-snp5.15:4.1_0": "Knowing it as its origin,",
    "ai-snp5.15:4.2_0": "Relishing is the fetters.",
    "ai-snp5.15:4.3_0": "The direct knowledge I understand this:",
    "ai-snp5.15:4.4_0": "sees clearly.",
    "ai-snp5.15:4.5_0": "that knowledge has been given",
    "ai-snp5.15:4.6_0": "is surrounded by a brahmin.”",
    "ai-snp5.15:4.7_0": "And the eleventh year camels."
}