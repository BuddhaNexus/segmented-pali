{
    "ai-tha-ap322:0.1_0": "Therāpadāna",
    "ai-tha-ap322:0.2_0": "Ārakkhadāyakavagga",
    "ai-tha-ap322:0.3_0": "10. Kumudamāliyattheraapadāna",
    "ai-tha-ap322:1.1_0": "O hero, with pray run back,",
    "ai-tha-ap322:1.2_0": "the great hermits.",
    "ai-tha-ap322:1.3_0": "seeing the great hero,",
    "ai-tha-ap322:1.4_0": "Kesamutta.",
    "ai-tha-ap322:2.1_0": "the chariot’s practice,",
    "ai-tha-ap322:2.2_0": "receiver of sacrifices,",
    "ai-tha-ap322:2.3_0": "Having accepted a pit,",
    "ai-tha-ap322:2.4_0": "I realized the best of Buddha.",
    "ai-tha-ap322:3.1_0": "At most eons",
    "ai-tha-ap322:3.2_0": "flowering along with flowers,",
    "ai-tha-ap322:3.3_0": "I’m not aware of any bad places",
    "ai-tha-ap322:3.4_0": "The fruit of the Buddha.",
    "ai-tha-ap322:4.1_0": "Attaining the four kinds of textual analysis",
    "ai-tha-ap322:4.2_0": "... “I attained the noble ones,” ...",
    "ai-tha-ap322:4.3_0": "and fulfilled the Buddha’s instructions.”",
    "ai-tha-ap322:5.1_0": "That is how these verses were recited by the senior venerable Kuṭiya.",
    "ai-tha-ap322:6.1_0": "Kuṇḍavāra.",
    "ai-tha-ap322:7.1_0": "the caretaker of the guarded by protection.",
    "ai-tha-ap322:8.1_0": "Inspiring places",
    "ai-tha-ap322:9.1_0": "that’s who’s guarded,",
    "ai-tha-ap322:9.2_0": "and perceiving the box;",
    "ai-tha-ap322:9.3_0": "flowers, food to eat,",
    "ai-tha-ap322:9.4_0": "perceiving sound,",
    "ai-tha-ap322:9.5_0": "Knot as a fish,",
    "ai-tha-ap322:9.6_0": "For seven Wisdom."
}