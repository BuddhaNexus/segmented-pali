{
    "ai-ja196:0.1_0": "Jātaka",
    "ai-ja196:0.2_0": "Dukanipāta",
    "ai-ja196:0.3_0": "Ruhakavagga",
    "ai-ja196:0.4_0": "6. Valāhakassajātaka",
    "ai-ja196:1.1_0": "“Those who don’t give the half-monthly instruction",
    "ai-ja196:1.2_0": "taught by the Buddha,",
    "ai-ja196:1.3_0": "They’ll go to ruin",
    "ai-ja196:1.4_0": "you protect yourself.",
    "ai-ja196:2.1_0": "Those who give you this advice,",
    "ai-ja196:2.2_0": "taught by the Buddha,",
    "ai-ja196:2.3_0": "safety they’ve gone far,",
    "ai-ja196:2.4_0": "like a trader in a fire.”",
    "ai-ja196:3.1_0": "like a rain-cloud of clouds."
}