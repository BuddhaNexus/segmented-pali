{
    "ja414:0.1_0": "The group of twelve on “as expected”",
    "ja414:0.2_0": "The Book of the Sevens",
    "ja414:0.3_0": "Foundation",
    "ja414:0.4_0": "wakeful while others sleep.",
    "ja414:1.1_0": "“Who sleeps wakeful,",
    "ja414:1.2_0": "anger is wakeful while others sleep.",
    "ja414:1.3_0": "Who knows this?",
    "ja414:1.4_0": "Who will block me from?”",
    "ja414:2.1_0": "I’m wakeful,",
    "ja414:2.2_0": "I’m wakeful while others sleep.",
    "ja414:2.3_0": "I’m aware of any doubt",
    "ja414:2.4_0": "I’m enjoying them.”",
    "ja414:3.1_0": "“What is wakeful, wakeful:",
    "ja414:3.2_0": "How is the wakeful?",
    "ja414:3.3_0": "How do you know this?",
    "ja414:3.4_0": "How am I supposed to speak to me?”",
    "ja414:4.1_0": "“Those who don’t understand the Dhamma",
    "ja414:4.2_0": "restraint and self-control,",
    "ja414:4.3_0": "Sleeping;",
    "ja414:4.4_0": "I am able to grow old.",
    "ja414:5.1_0": "They’ve dropped greed and hate,",
    "ja414:5.2_0": "whose ignorance has faded away,",
    "ja414:5.3_0": "wakeful while others sleep—",
    "ja414:5.4_0": "merely, god,",
    "ja414:6.1_0": "That’s how sleep while others wake.",
    "ja414:6.2_0": "That’s how wakeful while others sleep.",
    "ja414:6.3_0": "That’s how I recall that",
    "ja414:6.4_0": "so I’m coming!”",
    "ja414:7.1_0": "wakeful, wakeful,",
    "ja414:7.2_0": "wakeful while others sleep.",
    "ja414:7.3_0": "You know the pattern,",
    "ja414:7.4_0": "Please speak to me, Venerable.”",
    "ja414:8.1_0": "Nine wakeful."
}