{
    "ai-ja384:0.1_0": "Jātaka",
    "ai-ja384:0.2_0": "Chakkanipāta",
    "ai-ja384:0.3_0": "Avāriyavagga",
    "ai-ja384:0.4_0": "9. Dhammadhajajātaka",
    "ai-ja384:1.1_0": "“Seeing based on earth,",
    "ai-ja384:1.2_0": "follow your teaching!",
    "ai-ja384:1.3_0": "Who lives in line with pleasure,",
    "ai-ja384:1.4_0": "in this world or the next.”",
    "ai-ja384:2.1_0": "“This person is truly beautiful;",
    "ai-ja384:2.2_0": "that is the best of beings.",
    "ai-ja384:2.3_0": "Persist with one sensed",
    "ai-ja384:2.4_0": "for the welfare of the teaching.",
    "ai-ja384:3.1_0": "“May you give birth to ethics,",
    "ai-ja384:3.2_0": "for they praise",
    "ai-ja384:3.3_0": "choked jujube, and jujube—",
    "ai-ja384:3.4_0": "teaching Dhamma.",
    "ai-ja384:4.1_0": "if he speaks another;",
    "ai-ja384:4.2_0": "committing one offense?",
    "ai-ja384:4.3_0": "But we’re not a fool",
    "ai-ja384:4.4_0": "you haven’t determined.",
    "ai-ja384:5.1_0": "Today, mind, and baby,",
    "ai-ja384:5.2_0": "Channa is dark and bright.",
    "ai-ja384:5.3_0": "carrying faith in the village or town,",
    "ai-ja384:5.4_0": "is hard to find by a man.",
    "ai-ja384:6.1_0": "Virūpakkhas,",
    "ai-ja384:6.2_0": "Roja to the feet.",
    "ai-ja384:6.3_0": "For the sunshade would decreate me",
    "ai-ja384:6.4_0": "You shouldn’t live together.”",
    "ai-ja384:7.1_0": "The ninth sub-chapter on principle."
}