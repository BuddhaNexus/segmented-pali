{
    "ai-thag1.60:0.1_0": "Verses of the Senior Monks",
    "ai-thag1.60:0.2_0": "The Book of the Ones",
    "ai-thag1.60:0.3_0": "Chapter Six",
    "ai-thag1.60:0.4_0": "1.60. Sīvali",
    "ai-thag1.60:1.1_0": "My wishes—the purpose I had",
    "ai-thag1.60:1.2_0": "for entering this hut—came true.",
    "ai-thag1.60:1.3_0": "Abandoning the tendency to conceit,",
    "ai-thag1.60:1.4_0": "I’ll realize knowledge and liberation.",
    "ai-thag1.60:1.5_0": "…",
    "ai-thag1.60:1.6_0": "a senior monk.",
    "ai-thag1.60:1.7_0": "The sixth sub-chapter on graceful is finished.",
    "ai-thag1.60:2.0_0": "Inspiring places",
    "ai-thag1.60:2.1_0": "Godhika, Subāhu, and Subāhu;",
    "ai-thag1.60:2.2_0": "and Aṭṭhaka.",
    "ai-thag1.60:2.3_0": "The senior monk Rohitassas",
    "ai-thag1.60:2.4_0": "Two others, and a hut;",
    "ai-thag1.60:2.5_0": "Ramaṇīya,",
    "ai-thag1.60:2.6_0": "in the Kosalan lands."
}