{
  "ja459:0.1_0": "Jātaka",
  "ja459:0.2_0": "Ekādasakanipāta",
  "ja459:0.3_0": "Mātuposakavagga",
  "ja459:0.4_0": "5. Pānīyajātaka",
  "ja459:1.1_0": "“Mitto mittassa pānīyaṁ,",
  "ja459:1.2_0": "adinnaṁ paribhuñjisaṁ;",
  "ja459:1.3_0": "Tena pacchā vijigucchiṁ,",
  "ja459:1.4_0": "taṁ pāpaṁ pakataṁ mayā;",
  "ja459:1.5_0": "Mā puna akaraṁ pāpaṁ,",
  "ja459:1.6_0": "tasmā pabbajito ahaṁ.",
  "ja459:2.1_0": "Paradārañca disvāna,",
  "ja459:2.2_0": "chando me udapajjatha;",
  "ja459:2.3_0": "Tena pacchā vijigucchiṁ,",
  "ja459:2.4_0": "taṁ pāpaṁ pakataṁ mayā;",
  "ja459:2.5_0": "Mā puna akaraṁ pāpaṁ,",
  "ja459:2.6_0": "tasmā pabbajito ahaṁ.",
  "ja459:3.1_0": "Pitaraṁ me mahārāja,",
  "ja459:3.2_0": "corā agaṇhu kānane;",
  "ja459:3.3_0": "Tesāhaṁ pucchito jānaṁ,",
  "ja459:3.4_0": "aññathā naṁ viyākariṁ.",
  "ja459:4.1_0": "Tena pacchā vijigucchiṁ,",
  "ja459:4.2_0": "taṁ pāpaṁ pakataṁ mayā;",
  "ja459:4.3_0": "Mā puna akaraṁ pāpaṁ,",
  "ja459:4.4_0": "tasmā pabbajito ahaṁ.",
  "ja459:5.1_0": "Pāṇātipātamakaruṁ,",
  "ja459:5.2_0": "somayāge upaṭṭhite;",
  "ja459:5.3_0": "Tesāhaṁ samanuññāsiṁ.",
  "ja459:6.1_0": "Tena pacchā vijigucchiṁ,",
  "ja459:6.2_0": "taṁ pāpaṁ pakataṁ mayā;",
  "ja459:6.3_0": "Mā puna akaraṁ pāpaṁ,",
  "ja459:6.4_0": "tasmā pabbajito ahaṁ.",
  "ja459:7.1_0": "Surāmerayamādhukā,",
  "ja459:7.2_0": "ye janā paṭhamāsu no;",
  "ja459:7.3_0": "Bahūnaṁ te anatthāya,",
  "ja459:7.4_0": "majjapānamakappayuṁ;",
  "ja459:7.5_0": "Tesāhaṁ samanuññāsiṁ.",
  "ja459:8.1_0": "Tena pacchā vijigucchiṁ,",
  "ja459:8.2_0": "taṁ pāpaṁ pakataṁ mayā;",
  "ja459:8.3_0": "Mā puna akaraṁ pāpaṁ,",
  "ja459:8.4_0": "tasmā pabbajito ahaṁ”.",
  "ja459:9.1_0": "“Dhiratthu subahū kāme,",
  "ja459:9.2_0": "duggandhe bahukaṇṭake;",
  "ja459:9.3_0": "Ye ahaṁ paṭisevanto,",
  "ja459:9.4_0": "nālabhiṁ tādisaṁ sukhaṁ”.",
  "ja459:10.1_0": "“Mahassādā sukhā kāmā,",
  "ja459:10.2_0": "Natthi kāmā paraṁ sukhaṁ;",
  "ja459:10.3_0": "Ye kāme paṭisevanti,",
  "ja459:10.4_0": "Saggaṁ te upapajjare”.",
  "ja459:11.1_0": "“Appassādā dukhā kāmā,",
  "ja459:11.2_0": "natthi kāmā paraṁ dukhaṁ;",
  "ja459:11.3_0": "Ye kāme paṭisevanti,",
  "ja459:11.4_0": "nirayaṁ te upapajjare.",
  "ja459:12.1_0": "Asī yathā sunisito,",
  "ja459:12.2_0": "nettiṁsova supāyiko;",
  "ja459:12.3_0": "Sattīva urasi khittā,",
  "ja459:12.4_0": "kāmā dukkhatarā tato.",
  "ja459:13.1_0": "Aṅgārānaṁva jalitaṁ,",
  "ja459:13.2_0": "kāsuṁ sādhikaporisaṁ;",
  "ja459:13.3_0": "Phālaṁva divasantattaṁ,",
  "ja459:13.4_0": "kāmā dukkhatarā tato.",
  "ja459:14.1_0": "Visaṁ yathā halāhalaṁ,",
  "ja459:14.2_0": "telaṁ pakkuthitaṁ yathā;",
  "ja459:14.3_0": "Tambaloha vilīnaṁva,",
  "ja459:14.4_0": "kāmā dukkhatarā tato”ti.",
  "ja459:15.1_0": "Pānīyajātakaṁ pañcamaṁ."
}