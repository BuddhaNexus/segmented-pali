{
  "thag1.116:0.1_0": "Theragāthā 1.116",
  "thag1.116:0.2_0": "Ekakanipāta",
  "thag1.116:0.3_0": "Dvādasamavagga",
  "thag1.116:0.4_0": "Pārāpariyattheragāthā",
  "thag1.116:1.1_0": "“Chaphassāyatane hitvā,",
  "thag1.116:1.2_0": "guttadvāro susaṁvuto;",
  "thag1.116:1.3_0": "Aghamūlaṁ vamitvāna,",
  "thag1.116:1.4_0": "patto me āsavakkhayo”ti.",
  "thag1.116:1.5_0": "… Pārāpariyo thero …."
}