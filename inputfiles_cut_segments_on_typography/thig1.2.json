{
  "thig1.2:0.1_0": "Therīgāthā 1.2",
  "thig1.2:0.2_0": "Ekakanipāta",
  "thig1.2:0.3_0": "Muttātherīgāthā",
  "thig1.2:1.1_0": "“Mutte muccassu yogehi,",
  "thig1.2:1.2_0": "cando rāhuggahā iva;",
  "thig1.2:1.3_0": "Vippamuttena cittena,",
  "thig1.2:1.4_0": "anaṇā bhuñja piṇḍakan”ti.",
  "thig1.2:2.1_0": "Itthaṁ sudaṁ bhagavā muttaṁ sikkhamānaṁ imāya gāthāya abhiṇhaṁ ovadatīti."
}