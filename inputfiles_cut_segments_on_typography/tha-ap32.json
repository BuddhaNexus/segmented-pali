{
  "tha-ap32:0.1_0": "Therāpadāna",
  "tha-ap32:0.2_0": "Subhūtivagga",
  "tha-ap32:0.3_0": "10. Khomadāyakattheraapadāna",
  "tha-ap32:1.1_0": "“Nagare bandhumatiyā,",
  "tha-ap32:1.2_0": "ahosiṁ vāṇijo tadā;",
  "tha-ap32:1.3_0": "Teneva dāraṁ posemi,",
  "tha-ap32:1.4_0": "ropemi bījasampadaṁ.",
  "tha-ap32:2.1_0": "Rathiyaṁ paṭipannassa,",
  "tha-ap32:2.2_0": "vipassissa mahesino;",
  "tha-ap32:2.3_0": "Ekaṁ khomaṁ mayā dinnaṁ,",
  "tha-ap32:2.4_0": "kusalatthāya satthuno.",
  "tha-ap32:3.1_0": "Ekanavutito kappe,",
  "tha-ap32:3.2_0": "yaṁ khomamadadiṁ tadā;",
  "tha-ap32:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap32:3.4_0": "khomadānassidaṁ phalaṁ.",
  "tha-ap32:4.1_0": "Sattarase ito kappe,",
  "tha-ap32:4.2_0": "eko sindhavasandhano;",
  "tha-ap32:4.3_0": "Sattaratanasampanno,",
  "tha-ap32:4.4_0": "catudīpamhi issaro.",
  "tha-ap32:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap32:5.2_0": "Vimokkhāpi ca aṭṭhime;",
  "tha-ap32:5.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap32:5.4_0": "Kataṁ buddhassa sāsanaṁ”.",
  "tha-ap32:6.1_0": "Itthaṁ sudaṁ āyasmā khomadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap32:7.1_0": "Khomadāyakattherassāpadānaṁ dasamaṁ.",
  "tha-ap32:8.1_0": "Tassuddānaṁ",
  "tha-ap32:9.1_0": "Subhūti upavāno ca,",
  "tha-ap32:9.2_0": "saraṇo sīlagāhako;",
  "tha-ap32:9.3_0": "Annasaṁsāvako dhūpo,",
  "tha-ap32:9.4_0": "pulino uttiyena ca.",
  "tha-ap32:10.1_0": "Añjalī khomadāyī ca,",
  "tha-ap32:10.2_0": "daseva tatiye gaṇe;",
  "tha-ap32:10.3_0": "Pañcāsītisataṁ vuttā,",
  "tha-ap32:10.4_0": "gāthāyo sabbapiṇḍitā.",
  "tha-ap32:11.1_0": "Subhūtivaggo tatiyo.",
  "tha-ap32:12.1_0": "Catutthabhāṇavāraṁ."
}