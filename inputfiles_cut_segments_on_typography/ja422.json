{
  "ja422:0.1_0": "Jātaka",
  "ja422:0.2_0": "Aṭṭhakanipāta",
  "ja422:0.3_0": "Kaccānivagga",
  "ja422:0.4_0": "6. Cetiyajātaka",
  "ja422:1.1_0": "“Dhammo have hato hanti,",
  "ja422:1.2_0": "Nāhato hanti kiñcanaṁ;",
  "ja422:1.3_0": "Tasmā hi dhammaṁ na hane,",
  "ja422:1.4_0": "Mā tvaṁ dhammo hato hani.",
  "ja422:2.1_0": "Alikaṁ bhāsamānassa,",
  "ja422:2.2_0": "apakkamanti devatā;",
  "ja422:2.3_0": "Pūtikañca mukhaṁ vāti,",
  "ja422:2.4_0": "sakaṭṭhānā ca dhaṁsati;",
  "ja422:2.5_0": "Yo jānaṁ pucchito pañhaṁ,",
  "ja422:2.6_0": "aññathā naṁ viyākare.",
  "ja422:3.1_0": "Sace hi saccaṁ bhaṇasi,",
  "ja422:3.2_0": "hohi rāja yathā pure;",
  "ja422:3.3_0": "Musā ce bhāsase rāja,",
  "ja422:3.4_0": "bhūmiyaṁ tiṭṭha cetiya.",
  "ja422:4.1_0": "Akāle vassatī tassa,",
  "ja422:4.2_0": "kāle tassa na vassati;",
  "ja422:4.3_0": "Yo jānaṁ pucchito pañhaṁ,",
  "ja422:4.4_0": "aññathā naṁ viyākare.",
  "ja422:5.1_0": "Sace hi saccaṁ bhaṇasi,",
  "ja422:5.2_0": "hohi rāja yathā pure;",
  "ja422:5.3_0": "Musā ce bhāsase rāja,",
  "ja422:5.4_0": "bhūmiṁ pavisa cetiya.",
  "ja422:6.1_0": "Jivhā tassa dvidhā hoti,",
  "ja422:6.2_0": "uragasseva disampati;",
  "ja422:6.3_0": "Yo jānaṁ pucchito pañhaṁ,",
  "ja422:6.4_0": "aññathā naṁ viyākare.",
  "ja422:7.1_0": "Sace hi saccaṁ bhaṇasi,",
  "ja422:7.2_0": "hohi rāja yathā pure;",
  "ja422:7.3_0": "Musā ce bhāsase rāja,",
  "ja422:7.4_0": "bhiyyo pavisa cetiya.",
  "ja422:8.1_0": "Jivhā tassa na bhavati,",
  "ja422:8.2_0": "macchasseva disampati;",
  "ja422:8.3_0": "Yo jānaṁ pucchito pañhaṁ,",
  "ja422:8.4_0": "aññathā naṁ viyākare.",
  "ja422:9.1_0": "Sace hi saccaṁ bhaṇasi,",
  "ja422:9.2_0": "hohi rāja yathā pure;",
  "ja422:9.3_0": "Musā ce bhāsase rāja,",
  "ja422:9.4_0": "bhiyyo pavisa cetiya.",
  "ja422:10.1_0": "Thiyova tassa jāyanti,",
  "ja422:10.2_0": "na pumā jāyare kule;",
  "ja422:10.3_0": "Yo jānaṁ pucchito pañhaṁ,",
  "ja422:10.4_0": "aññathā naṁ viyākare.",
  "ja422:11.1_0": "Sace hi saccaṁ bhaṇasi,",
  "ja422:11.2_0": "hohi rāja yathā pure;",
  "ja422:11.3_0": "Musā ce bhāsase rāja,",
  "ja422:11.4_0": "bhiyyo pavisa cetiya.",
  "ja422:12.1_0": "Puttā tassa na bhavanti,",
  "ja422:12.2_0": "pakkamanti disodisaṁ;",
  "ja422:12.3_0": "Yo jānaṁ pucchito pañhaṁ,",
  "ja422:12.4_0": "aññathā naṁ viyākare.",
  "ja422:13.1_0": "Sace hi saccaṁ bhaṇasi,",
  "ja422:13.2_0": "hohi rāja yathā pure;",
  "ja422:13.3_0": "Musā ce bhāsase rāja,",
  "ja422:13.4_0": "bhiyyo pavisa cetiya”.",
  "ja422:14.1_0": "“Sa rājā isinā satto,",
  "ja422:14.2_0": "antalikkhacaro pure;",
  "ja422:14.3_0": "Pāvekkhi pathaviṁ cecco,",
  "ja422:14.4_0": "hīnatto patva pariyāyaṁ.",
  "ja422:15.1_0": "Tasmā hi chandāgamanaṁ,",
  "ja422:15.2_0": "Nappasaṁsanti paṇḍitā;",
  "ja422:15.3_0": "Aduṭṭhacitto bhāseyya,",
  "ja422:15.4_0": "Giraṁ saccūpasaṁhitanti”.",
  "ja422:16.1_0": "Cetiyajātakaṁ chaṭṭhaṁ."
}