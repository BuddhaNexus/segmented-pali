{
  "thag1.21:0.1_0": "Theragāthā 1.21",
  "thag1.21:0.2_0": "Ekakanipāta",
  "thag1.21:0.3_0": "Tatiyavagga",
  "thag1.21:0.4_0": "Nigrodhattheragāthā",
  "thag1.21:1.1_0": "“Nāhaṁ bhayassa bhāyāmi,",
  "thag1.21:1.2_0": "Satthā no amatassa kovido;",
  "thag1.21:1.3_0": "Yattha bhayaṁ nāvatiṭṭhati,",
  "thag1.21:1.4_0": "Tena maggena vajanti bhikkhavo”ti.",
  "thag1.21:1.5_0": "… Nigrodho thero …."
}