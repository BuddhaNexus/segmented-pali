{
  "ja378:0.1_0": "Jātaka",
  "ja378:0.2_0": "Chakkanipāta",
  "ja378:0.3_0": "Avāriyavagga",
  "ja378:0.4_0": "3. Darīmukhajātaka",
  "ja378:1.1_0": "“Paṅko ca kāmā palipo ca kāmā,",
  "ja378:1.2_0": "Bhayañca metaṁ timūlaṁ pavuttaṁ;",
  "ja378:1.3_0": "Rajo ca dhūmo ca mayā pakāsitā,",
  "ja378:1.4_0": "Hitvā tuvaṁ pabbaja brahmadatta”.",
  "ja378:2.1_0": "“Gadhito ca ratto ca adhimucchito ca,",
  "ja378:2.2_0": "Kāmesvahaṁ brāhmaṇa bhiṁsarūpaṁ;",
  "ja378:2.3_0": "Taṁ nussahe jīvikattho pahātuṁ,",
  "ja378:2.4_0": "Kāhāmi puññāni anappakāni”.",
  "ja378:3.1_0": "“Yo atthakāmassa hitānukampino,",
  "ja378:3.2_0": "Ovajjamāno na karoti sāsanaṁ;",
  "ja378:3.3_0": "Idameva seyyo iti maññamāno,",
  "ja378:3.4_0": "Punappunaṁ gabbhamupeti mando.",
  "ja378:4.1_0": "So ghorarūpaṁ nirayaṁ upeti,",
  "ja378:4.2_0": "Subhāsubhaṁ muttakarīsapūraṁ;",
  "ja378:4.3_0": "Sattā sakāye na jahanti giddhā,",
  "ja378:4.4_0": "Ye honti kāmesu avītarāgā”.",
  "ja378:5.1_0": "“Mīḷhena littā ruhirena makkhitā,",
  "ja378:5.2_0": "Semhena littā upanikkhamanti;",
  "ja378:5.3_0": "Yaṁ yañhi kāyena phusanti tāvade,",
  "ja378:5.4_0": "Sabbaṁ asātaṁ dukhameva kevalaṁ.",
  "ja378:6.1_0": "Disvā vadāmi na hi aññato savaṁ,",
  "ja378:6.2_0": "Pubbenivāsaṁ bahukaṁ sarāmi;",
  "ja378:6.3_0": "Citrāhi gāthāhi subhāsitāhi,",
  "ja378:6.4_0": "Darīmukho nijjhāpayi sumedhan”ti.",
  "ja378:7.1_0": "Darīmukhajātakaṁ tatiyaṁ."
}