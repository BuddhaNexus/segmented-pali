{
  "ja293:0.1_0": "Jātaka",
  "ja293:0.2_0": "Tikanipāta",
  "ja293:0.3_0": "Kumbhavagga",
  "ja293:0.4_0": "3. Kāyanibbindajātaka",
  "ja293:1.1_0": "“Phuṭṭhassa me aññatarena byādhinā,",
  "ja293:1.2_0": "Rogena bāḷhaṁ dukhitassa ruppato;",
  "ja293:1.3_0": "Parisussati khippamidaṁ kaḷevaraṁ,",
  "ja293:1.4_0": "Pupphaṁ yathā paṁsuni ātape kataṁ.",
  "ja293:2.1_0": "Ajaññaṁ jaññasaṅkhātaṁ,",
  "ja293:2.2_0": "asuciṁ sucisammataṁ;",
  "ja293:2.3_0": "Nānākuṇapaparipūraṁ,",
  "ja293:2.4_0": "jaññarūpaṁ apassato.",
  "ja293:3.1_0": "Dhiratthumaṁ āturaṁ pūtikāyaṁ,",
  "ja293:3.2_0": "Jegucchiyaṁ assuciṁ byādhidhammaṁ;",
  "ja293:3.3_0": "Yatthappamattā adhimucchitā pajā,",
  "ja293:3.4_0": "Hāpenti maggaṁ sugatūpapattiyā”ti.",
  "ja293:4.1_0": "Kāyanibbindajātakaṁ tatiyaṁ."
}