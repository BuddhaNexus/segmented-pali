{
  "tha-ap423:0.1_0": "Therāpadāna",
  "tha-ap423:0.2_0": "Sakiṁsammajjakavagga",
  "tha-ap423:0.3_0": "1. Sakiṁsammajjakattheraapadāna",
  "tha-ap423:1.1_0": "“Vipassino bhagavato,",
  "tha-ap423:1.2_0": "pāṭaliṁ bodhimuttamaṁ;",
  "tha-ap423:1.3_0": "Disvāva taṁ pādapaggaṁ,",
  "tha-ap423:1.4_0": "tattha cittaṁ pasādayiṁ.",
  "tha-ap423:2.1_0": "Sammajjaniṁ gahetvāna,",
  "tha-ap423:2.2_0": "bodhiṁ sammajji tāvade;",
  "tha-ap423:2.3_0": "Sammajjitvāna taṁ bodhiṁ,",
  "tha-ap423:2.4_0": "avandiṁ pāṭaliṁ ahaṁ.",
  "tha-ap423:3.1_0": "Tattha cittaṁ pasādetvā,",
  "tha-ap423:3.2_0": "sire katvāna añjaliṁ;",
  "tha-ap423:3.3_0": "Namassamāno taṁ bodhiṁ,",
  "tha-ap423:3.4_0": "gañchiṁ paṭikuṭiṁ ahaṁ.",
  "tha-ap423:4.1_0": "Tādimaggena gacchāmi,",
  "tha-ap423:4.2_0": "saranto bodhimuttamaṁ;",
  "tha-ap423:4.3_0": "Ajagaro maṁ pīḷesi,",
  "tha-ap423:4.4_0": "ghorarūpo mahabbalo.",
  "tha-ap423:5.1_0": "Āsanne me kataṁ kammaṁ,",
  "tha-ap423:5.2_0": "phalena tosayī mamaṁ;",
  "tha-ap423:5.3_0": "Kaḷevaraṁ me gilati,",
  "tha-ap423:5.4_0": "devaloke ramāmahaṁ.",
  "tha-ap423:6.1_0": "Anāvilaṁ mama cittaṁ,",
  "tha-ap423:6.2_0": "visuddhaṁ paṇḍaraṁ sadā;",
  "tha-ap423:6.3_0": "Sokasallaṁ na jānāmi,",
  "tha-ap423:6.4_0": "cittasantāpanaṁ mama.",
  "tha-ap423:7.1_0": "Kuṭṭhaṁ gaṇḍo kilāso ca,",
  "tha-ap423:7.2_0": "apamāro vitacchikā;",
  "tha-ap423:7.3_0": "Daddu kaṇḍu ca me natthi,",
  "tha-ap423:7.4_0": "phalaṁ sammajjanāyidaṁ.",
  "tha-ap423:8.1_0": "Soko ca paridevo ca,",
  "tha-ap423:8.2_0": "hadaye me na vijjati;",
  "tha-ap423:8.3_0": "Abhantaṁ ujukaṁ cittaṁ,",
  "tha-ap423:8.4_0": "phalaṁ sammajjanāyidaṁ.",
  "tha-ap423:9.1_0": "Samādhīsu na majjāmi,",
  "tha-ap423:9.2_0": "visadaṁ hoti mānasaṁ;",
  "tha-ap423:9.3_0": "Yaṁ yaṁ samādhimicchāmi,",
  "tha-ap423:9.4_0": "so so sampajjate mamaṁ.",
  "tha-ap423:10.1_0": "Rajanīye na rajjāmi,",
  "tha-ap423:10.2_0": "atho dussaniyesu ca;",
  "tha-ap423:10.3_0": "Mohanīye na muyhāmi,",
  "tha-ap423:10.4_0": "phalaṁ sammajjanāyidaṁ.",
  "tha-ap423:11.1_0": "Ekanavutito kappe,",
  "tha-ap423:11.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap423:11.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap423:11.4_0": "phalaṁ sammajjanāyidaṁ.",
  "tha-ap423:12.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap423:12.2_0": "bhavā sabbe samūhatā;",
  "tha-ap423:12.3_0": "Nāgova bandhanaṁ chetvā,",
  "tha-ap423:12.4_0": "viharāmi anāsavo.",
  "tha-ap423:13.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap423:13.2_0": "mama buddhassa santike;",
  "tha-ap423:13.3_0": "Tisso vijjā anuppattā,",
  "tha-ap423:13.4_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap423:14.1_0": "Paṭisambhidā catasso,",
  "tha-ap423:14.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap423:14.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap423:14.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap423:15.1_0": "Itthaṁ sudaṁ āyasmā sakiṁsammajjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap423:16.1_0": "Sakiṁsammajjakattherassāpadānaṁ paṭhamaṁ."
}