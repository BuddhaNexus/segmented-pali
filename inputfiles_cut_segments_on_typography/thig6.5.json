{
  "thig6.5:0.1_0": "Therīgāthā 6.5",
  "thig6.5:0.2_0": "Chakkanipāta",
  "thig6.5:0.3_0": "Anopamātherīgāthā",
  "thig6.5:1.1_0": "“Ucce kule ahaṁ jātā,",
  "thig6.5:1.2_0": "bahuvitte mahaddhane;",
  "thig6.5:1.3_0": "Vaṇṇarūpena sampannā,",
  "thig6.5:1.4_0": "dhītā majjhassa atrajā.",
  "thig6.5:2.1_0": "Patthitā rājaputtehi,",
  "thig6.5:2.2_0": "seṭṭhiputtehi gijjhitā;",
  "thig6.5:2.3_0": "Pitu me pesayī dūtaṁ,",
  "thig6.5:2.4_0": "detha mayhaṁ anopamaṁ.",
  "thig6.5:3.1_0": "Yattakaṁ tulitā esā,",
  "thig6.5:3.2_0": "tuyhaṁ dhītā anopamā;",
  "thig6.5:3.3_0": "Tato aṭṭhaguṇaṁ dassaṁ,",
  "thig6.5:3.4_0": "hiraññaṁ ratanāni ca.",
  "thig6.5:4.1_0": "Sāhaṁ disvāna sambuddhaṁ,",
  "thig6.5:4.2_0": "lokajeṭṭhaṁ anuttaraṁ;",
  "thig6.5:4.3_0": "Tassa pādāni vanditvā,",
  "thig6.5:4.4_0": "ekamantaṁ upāvisiṁ.",
  "thig6.5:5.1_0": "So me dhammamadesesi,",
  "thig6.5:5.2_0": "anukampāya gotamo;",
  "thig6.5:5.3_0": "Nisinnā āsane tasmiṁ,",
  "thig6.5:5.4_0": "phusayiṁ tatiyaṁ phalaṁ.",
  "thig6.5:6.1_0": "Tato kesāni chetvāna,",
  "thig6.5:6.2_0": "pabbajiṁ anagāriyaṁ;",
  "thig6.5:6.3_0": "Ajja me sattamī ratti,",
  "thig6.5:6.4_0": "yato taṇhā visositā”ti.",
  "thig6.5:7.1_0": "… Anopamā therī …."
}