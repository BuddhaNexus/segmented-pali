{
  "thag2.35:0.1_0": "Theragāthā 2.35",
  "thag2.35:0.2_0": "Dukanipāta",
  "thag2.35:0.3_0": "Catutthavagga",
  "thag2.35:0.4_0": "Sambulakaccānattheragāthā",
  "thag2.35:1.1_0": "“Devo ca vassati, devo ca gaḷagaḷāyati,",
  "thag2.35:1.2_0": "Ekako cāhaṁ bherave bile viharāmi;",
  "thag2.35:1.3_0": "Tassa mayhaṁ ekakassa bherave bile viharato,",
  "thag2.35:1.4_0": "Natthi bhayaṁ vā chambhitattaṁ vā lomahaṁso vā.",
  "thag2.35:2.1_0": "Dhammatā mamesā yassa me,",
  "thag2.35:2.2_0": "Ekakassa bherave bile;",
  "thag2.35:2.3_0": "Viharato natthi bhayaṁ vā,",
  "thag2.35:2.4_0": "Chambhitattaṁ vā lomahaṁso vā”ti.",
  "thag2.35:2.5_0": "… Sambulakaccāno thero …."
}