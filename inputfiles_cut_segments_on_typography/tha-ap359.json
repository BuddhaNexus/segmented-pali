{
  "tha-ap359:0.1_0": "Therāpadāna",
  "tha-ap359:0.2_0": "Saddasaññakavagga",
  "tha-ap359:0.3_0": "7 Supuṭakapūjakattheraapadāna",
  "tha-ap359:1.1_0": "“Divāvihārā nikkhanto,",
  "tha-ap359:1.2_0": "vipassī lokanāyako;",
  "tha-ap359:1.3_0": "Bhikkhāya vicaranto so,",
  "tha-ap359:1.4_0": "mama santikupāgami.",
  "tha-ap359:2.1_0": "Tato patīto sumano,",
  "tha-ap359:2.2_0": "buddhaseṭṭhassa tādino;",
  "tha-ap359:2.3_0": "Loṇasupuṭakaṁ datvā,",
  "tha-ap359:2.4_0": "kappaṁ saggamhi modahaṁ.",
  "tha-ap359:3.1_0": "Ekanavutito kappe,",
  "tha-ap359:3.2_0": "yaṁ puṭakamadāsahaṁ;",
  "tha-ap359:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap359:3.4_0": "puṭakassa idaṁ phalaṁ.",
  "tha-ap359:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap359:4.2_0": "…pe…",
  "tha-ap359:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap359:5.1_0": "Itthaṁ sudaṁ āyasmā supuṭakapūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap359:6.1_0": "Supuṭakapūjakattherassāpadānaṁ sattamaṁ."
}