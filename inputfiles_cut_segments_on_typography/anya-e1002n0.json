{
  "anya-e1002n0:0_0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1002n0:1_0": "Nītimañjarī",
  "anya-e1002n0:2_0": "1.",
  "anya-e1002n0:3_0": "Kulakkhaye vinassanti,",
  "anya-e1002n0:4_0": "Kuladhammā sanantanā;",
  "anya-e1002n0:5_0": "Dhamme naṭṭhe kulaṃ sabbaṃ,",
  "anya-e1002n0:6_0": "Adhammo abhibhū khalaṃ.",
  "anya-e1002n0:7_0": "2.",
  "anya-e1002n0:8_0": "Adhammābhibhavā dantā,",
  "anya-e1002n0:9_0": "Padussanti kulitthiyo;",
  "anya-e1002n0:10_0": "Thīsu duṭṭhā sva dhammena,",
  "anya-e1002n0:11_0": "Jāyate vaṇṇasaṅkaro.",
  "anya-e1002n0:12_0": "3.",
  "anya-e1002n0:13_0": "Piyaṃ bhāse guṇaggāho,",
  "anya-e1002n0:14_0": "Sūro siyā vikantano;",
  "anya-e1002n0:15_0": "Dātā candasamā nārī,",
  "anya-e1002n0:16_0": "Diṭṭhaṃ diṭṭhaṃ nahāsaye.",
  "anya-e1002n0:17_0": "4.",
  "anya-e1002n0:18_0": "Kutotthi kumitte saccaṃ,",
  "anya-e1002n0:19_0": "Kudāre rativaḍḍhanaṃ;",
  "anya-e1002n0:20_0": "Kudesamhi mano rammaṃ,",
  "anya-e1002n0:21_0": "Kurāje bhogasampadaṃ.",
  "anya-e1002n0:22_0": "Saṅketeva amittasmiṃ,",
  "anya-e1002n0:23_0": "Mittasmiṃ pi navissase;",
  "anya-e1002n0:24_0": "Abhayā bhaya muppannaṃ,",
  "anya-e1002n0:25_0": "Api mūlāni kantati.",
  "anya-e1002n0:26_0": "Adiṭṭhova paro seyyo,",
  "anya-e1002n0:27_0": "Dummitto no vissāsiko.",
  "anya-e1002n0:28_0": "Aggihomaphalaṃ vedo,",
  "anya-e1002n0:29_0": "Satthaṃsīlaphalaṃ mataṃ;",
  "anya-e1002n0:30_0": "Ratiputtaphalaṃ nārī,",
  "anya-e1002n0:31_0": "Dānabhuttiphalaṃ dhanaṃ.",
  "anya-e1002n0:32_0": "Asaccaṃ sāhasaṃ māyā,",
  "anya-e1002n0:33_0": "Mūḷhatta ma tilobhatā;",
  "anya-e1002n0:34_0": "Asocaṃ niddayattañca,",
  "anya-e1002n0:35_0": "Thīnaṃ dosā sabhāvajā.",
  "anya-e1002n0:36_0": "Jāyāya bhattuno bhāro,",
  "anya-e1002n0:37_0": "Sissena guruno kato;",
  "anya-e1002n0:38_0": "Amaccakehi rājassa,",
  "anya-e1002n0:39_0": "Pitarānaṃ nijenaca.",
  "anya-e1002n0:40_0": "5.",
  "anya-e1002n0:41_0": "Uyyamena hi sijjhanti,",
  "anya-e1002n0:42_0": "Kammāni na manorathā;",
  "anya-e1002n0:43_0": "Na hi suttassa sīhassa,",
  "anya-e1002n0:44_0": "Pavīsanti mukhe migā.",
  "anya-e1002n0:45_0": "Atisītaṃ atiuṇhaṃ,",
  "anya-e1002n0:46_0": "Atisāyamidaṃ ahu;",
  "anya-e1002n0:47_0": "Iti visaṭṭhakammante,",
  "anya-e1002n0:48_0": "Khaṇā accenti māṇave.",
  "anya-e1002n0:49_0": "Ādānassa padānassa,",
  "anya-e1002n0:50_0": "Kattabbassa ca kammuno;",
  "anya-e1002n0:51_0": "Khippaṃ akayyamānassa,",
  "anya-e1002n0:52_0": "Kāle pivati sampadaṃ.",
  "anya-e1002n0:53_0": "Nādabbe nihitā kāci,",
  "anya-e1002n0:54_0": "Kriyā phalavatī bhave;",
  "anya-e1002n0:55_0": "Nabyāpārasatenāpi,",
  "anya-e1002n0:56_0": "Sukova pāṭhate bako.",
  "anya-e1002n0:57_0": "Yo dandhakāle tarati,",
  "anya-e1002n0:58_0": "Taraṇīye ca dandhaye;",
  "anya-e1002n0:59_0": "Sukkhapaṇṇaṃ va akkamma,",
  "anya-e1002n0:60_0": "Atthaṃ bhañjati attano.",
  "anya-e1002n0:61_0": "6.",
  "anya-e1002n0:62_0": "Yaṃ dadāti yaṃ bhuñjati,",
  "anya-e1002n0:63_0": "Tadeva dhanino dhanaṃ;",
  "anya-e1002n0:64_0": "Aññe matassa kīḷanti,",
  "anya-e1002n0:65_0": "Dārehipi dhanehipi.",
  "anya-e1002n0:66_0": "Dānopabhogahīnena,",
  "anya-e1002n0:67_0": "Dhanena dhanino sukhaṃ;",
  "anya-e1002n0:68_0": "Ko viseso daliddassa,",
  "anya-e1002n0:69_0": "Adhikaṃ dhanarakkhaṇaṃ.",
  "anya-e1002n0:70_0": "Nijasokhyaṃ nirundhanto,",
  "anya-e1002n0:71_0": "Nīcabhogo mitampaco;",
  "anya-e1002n0:72_0": "Dhanaṃ sañcayate yo so,",
  "anya-e1002n0:73_0": "Parabhāravaho pasu.",
  "anya-e1002n0:74_0": "Yaṃ ussukā saṅkharonti,",
  "anya-e1002n0:75_0": "Alakkhikā bahuṃ dhanaṃ;",
  "anya-e1002n0:76_0": "Sippavanto asippāvā,",
  "anya-e1002n0:77_0": "Lakkhi vā tāni bhuñjati.",
  "anya-e1002n0:78_0": "7.",
  "anya-e1002n0:79_0": "Sampatyaṃ mahataṃ cittaṃ,",
  "anya-e1002n0:80_0": "Bhave uppale komalaṃ;",
  "anya-e1002n0:81_0": "Vipatyaṃca mahāsela,",
  "anya-e1002n0:82_0": "Silāsaṅghātakakkasaṃ.",
  "anya-e1002n0:83_0": "8.",
  "anya-e1002n0:84_0": "Asambhabyaguṇaṃ thutvā,",
  "anya-e1002n0:85_0": "Khedo mudhāva jāyate;",
  "anya-e1002n0:86_0": "Avhāyaṃ canda mu llokya,",
  "anya-e1002n0:87_0": "Nacandota mu pāgamī.",
  "anya-e1002n0:88_0": "9.",
  "anya-e1002n0:89_0": "Saccaṃ mukhamhi dhāreyya,",
  "anya-e1002n0:90_0": "Kaṇṇe sutaṃ bhuje jayaṃ;",
  "anya-e1002n0:91_0": "Hadayamhi khamaṃ vīraṃ,",
  "anya-e1002n0:92_0": "Lokādāsaṃca locane.",
  "anya-e1002n0:93_0": "Saddamattaṃ naphandeyya,",
  "anya-e1002n0:94_0": "Aññatvā saddakāraṇaṃ;",
  "anya-e1002n0:95_0": "Saddahetuṃ pariññāya,",
  "anya-e1002n0:96_0": "Pamodo vā bhayo tathā.",
  "anya-e1002n0:97_0": "Sabbasuta ma dhīyeyya,",
  "anya-e1002n0:98_0": "Hīnamukkaṭṭhamajjhimaṃ.",
  "anya-e1002n0:99_0": "10.",
  "anya-e1002n0:100_0": "Dunnāriyā kulaṃ suddhaṃ,",
  "anya-e1002n0:101_0": "Putto nassati lālanā;",
  "anya-e1002n0:102_0": "Samiddhi anayā bandhu,",
  "anya-e1002n0:103_0": "Pavāsā madanā hirī.",
  "anya-e1002n0:104_0": "Lālaye pañcavassāni,",
  "anya-e1002n0:105_0": "Dasavassāni tālaye;",
  "anya-e1002n0:106_0": "Pattetu soḷasevasse,",
  "anya-e1002n0:107_0": "Puttaṃ mittaṃva ācare.",
  "anya-e1002n0:108_0": "Lālane bahavo dosā,",
  "anya-e1002n0:109_0": "Lālane bahavo guṇā.",
  "anya-e1002n0:110_0": "Pāpā nivārayati yojayate hitāya,",
  "anya-e1002n0:111_0": "Guyhāni gūhati guṇaṃ pakaṭīkaroti;",
  "anya-e1002n0:112_0": "Āpattikañca najahāti dadāti kāle,",
  "anya-e1002n0:113_0": "Sammitta lakkhaṇamidaṃ pavadanti santo.",
  "anya-e1002n0:114_0": "11.",
  "anya-e1002n0:115_0": "Dujjano jīyate yutyā,",
  "anya-e1002n0:116_0": "Niggahena nadhīmatā;",
  "anya-e1002n0:117_0": "Nipātyate mahārukkho,",
  "anya-e1002n0:118_0": "Tassamīpa khatikkhayā.",
  "anya-e1002n0:119_0": "Vane migāca luddhānaṃ,",
  "anya-e1002n0:120_0": "Dujjanānañca sajjanā;",
  "anya-e1002n0:121_0": "Akāraṇaverī honti,",
  "anya-e1002n0:122_0": "Tiṇabhakkhā supesalā.",
  "anya-e1002n0:123_0": "Pādalaggaṃ karaṭṭhena,",
  "anya-e1002n0:124_0": "Kaṇḍakeneva kaṇḍakaṃ.",
  "anya-e1002n0:125_0": "Bālaṃ napasse nasuṇe,",
  "anya-e1002n0:126_0": "Nacabālena saṃvase;",
  "anya-e1002n0:127_0": "Bālenāllāpasallāpaṃ,",
  "anya-e1002n0:128_0": "Nakare nacarocaye.",
  "anya-e1002n0:129_0": "12.",
  "anya-e1002n0:130_0": "Upa kattuṃ yathā khuddo,",
  "anya-e1002n0:131_0": "Samattho natathāmahā;",
  "anya-e1002n0:132_0": "Kūpo hi hanti pipāsaṃ,",
  "anya-e1002n0:133_0": "Natu pāyo mahambudhi.",
  "anya-e1002n0:134_0": "13.",
  "anya-e1002n0:135_0": "Ādānassa padānassa,",
  "anya-e1002n0:136_0": "Kattabbassaca kammuno;",
  "anya-e1002n0:137_0": "Khippaṃ akaramānassa,",
  "anya-e1002n0:138_0": "Kālo bhakkhati taṃ rasaṃ.",
  "anya-e1002n0:139_0": "Nakkhattaṃ paṭimānentaṃ,",
  "anya-e1002n0:140_0": "Attho bālaṃ upajjhagā;",
  "anya-e1002n0:141_0": "Attho atthassa nakkhattaṃ,",
  "anya-e1002n0:142_0": "Kiṃ karissanti tārakā.",
  "anya-e1002n0:143_0": "Ajarāmarova pañño,",
  "anya-e1002n0:144_0": "Vijjamatthañca cintaye;",
  "anya-e1002n0:145_0": "Gahitoviya kesesu,",
  "anya-e1002n0:146_0": "Maccunā dhammamācare.",
  "anya-e1002n0:147_0": "14.",
  "anya-e1002n0:148_0": "Vajjā gurūca mantīca,",
  "anya-e1002n0:149_0": "Tayo raṭṭhābhisaṅkhatā;",
  "anya-e1002n0:150_0": "Jīvīta dakkha kosānaṃ,",
  "anya-e1002n0:151_0": "Vaḍḍhanā nāsanāca te.",
  "anya-e1002n0:152_0": "15.",
  "anya-e1002n0:153_0": "Thirena kammaṃ vaḍḍhati,",
  "anya-e1002n0:154_0": "Athirena turena no;",
  "anya-e1002n0:155_0": "Phalanti samaye rukkhā,",
  "anya-e1002n0:156_0": "Sittāpi bahuvārinā.",
  "anya-e1002n0:157_0": "Vāyāmetheva puriso,",
  "anya-e1002n0:158_0": "Nanibbindeyya paṇḍito.",
  "anya-e1002n0:159_0": "Payatano tādiso neva,",
  "anya-e1002n0:160_0": "Kayyo yena phalaṃ nahi;",
  "anya-e1002n0:161_0": "Selagge kūpakhaṇanā,",
  "anya-e1002n0:162_0": "Kathaṃ toyasamāgamo.",
  "anya-e1002n0:163_0": "Ñāṇaṅkusena sammaggaṃ,",
  "anya-e1002n0:164_0": "Niyyatyussāhakuñjaro.",
  "anya-e1002n0:165_0": "Asamekkhitakammantaṃ,",
  "anya-e1002n0:166_0": "Turitābhi nipātinaṃ;",
  "anya-e1002n0:167_0": "Tānikammāni tappenti,",
  "anya-e1002n0:168_0": "Uṇhaṃ va jjhohitaṃ mukhe.",
  "anya-e1002n0:169_0": "16.",
  "anya-e1002n0:170_0": "Chaddosā puriseneha,",
  "anya-e1002n0:171_0": "Hātabbā bhūtimicchantā;",
  "anya-e1002n0:172_0": "Niddā majjaṃ bhayaṃ kodho,",
  "anya-e1002n0:173_0": "Ālasyaṃ dīghasuttatā.",
  "anya-e1002n0:174_0": "Na divā suppasīlena,",
  "anya-e1002n0:175_0": "Rattimuṭṭhānadessinā;",
  "anya-e1002n0:176_0": "Niccasoṇḍena mattena,",
  "anya-e1002n0:177_0": "Sakkā āvasituṃ gharaṃ.",
  "anya-e1002n0:178_0": "Abhetabbamhi bhāyanti,",
  "anya-e1002n0:179_0": "Bhāyitabbe nabhāyare;",
  "anya-e1002n0:180_0": "Bhayābhaya vimuḷhā te,",
  "anya-e1002n0:181_0": "Jimhānugā ujuñjahā.",
  "anya-e1002n0:182_0": "Yassa manussabhūtassa,",
  "anya-e1002n0:183_0": "Natthi bhogāca sippakaṃ;",
  "anya-e1002n0:184_0": "Kiṃ phalaṃ tassa mānussaṃ,",
  "anya-e1002n0:185_0": "Dvipādaṭṭho hi so migo.",
  "anya-e1002n0:186_0": "17.",
  "anya-e1002n0:187_0": "Nānopāyova kattabbo,",
  "anya-e1002n0:188_0": "Sace bhaveyya attano;",
  "anya-e1002n0:189_0": "Atthasiddhi yathākāmaṃ,",
  "anya-e1002n0:190_0": "Upāyo hi hitañjaso.",
  "anya-e1002n0:191_0": "Lañjadānabālisena,",
  "anya-e1002n0:192_0": "Kūṭaḍḍakāradhīvarā;",
  "anya-e1002n0:193_0": "Vinicchayamahāmacchaṃ,",
  "anya-e1002n0:194_0": "Oṭṭenti lobhasāgare.",
  "anya-e1002n0:195_0": "Yassete caturo dhammā,",
  "anya-e1002n0:196_0": "Vānarinda yathātava;",
  "anya-e1002n0:197_0": "Saccaṃ dhammo dhīti cāgo,",
  "anya-e1002n0:198_0": "Diṭṭhaṃ so ativattati.",
  "anya-e1002n0:199_0": "18.",
  "anya-e1002n0:200_0": "Vidvāca ratanaṃ nārī,",
  "anya-e1002n0:201_0": "Vīṇā sātthaṃ giraṃmahī;",
  "anya-e1002n0:202_0": "Guṇavisesa māgamma,",
  "anya-e1002n0:203_0": "Guṇāni aguṇānica.",
  "anya-e1002n0:204_0": "Dhanavā balavā loke,",
  "anya-e1002n0:205_0": "Dhanā bhavati paṇḍito.",
  "anya-e1002n0:206_0": "Sumane nissito kīṭo,",
  "anya-e1002n0:207_0": "Nigguṇo hīnako sayaṃ;",
  "anya-e1002n0:208_0": "Taṃ pupphehi maṇḍentānaṃ,",
  "anya-e1002n0:209_0": "Raññaṃ siropi rohati.",
  "anya-e1002n0:210_0": "Alakkhikehi sañcītā,",
  "anya-e1002n0:211_0": "Dhanabhogāca cintitā;",
  "anya-e1002n0:212_0": "Lakkhikassa bhavantete,",
  "anya-e1002n0:213_0": "Lakkhivā suṭṭhubhuñjati.",
  "anya-e1002n0:214_0": "Khattiyo seṭṭho jane tasmiṃ,",
  "anya-e1002n0:215_0": "Yo gottapaṭisārino;",
  "anya-e1002n0:216_0": "Vijjācaraṇasampanno,",
  "anya-e1002n0:217_0": "So seṭṭho devamānuse.",
  "anya-e1002n0:218_0": "Visāpi amataṃ gaṇhe,",
  "anya-e1002n0:219_0": "Gūthato maṇimuttamaṃ;",
  "anya-e1002n0:220_0": "Kaṇṭakapādapā pupphaṃ,",
  "anya-e1002n0:221_0": "Thirataṃ dukkulā varaṃ.",
  "anya-e1002n0:222_0": "Dhanissarādiguṇommi -",
  "anya-e1002n0:223_0": "Vegena vāhitā pajā.",
  "anya-e1002n0:224_0": "19.",
  "anya-e1002n0:225_0": "Yassa tthi satataṃ mettā,",
  "anya-e1002n0:226_0": "Sabbalokasuvallabhā;",
  "anya-e1002n0:227_0": "Kūpāyate samuddopi,",
  "anya-e1002n0:228_0": "Aggi tassa jalāyate.",
  "anya-e1002n0:229_0": "20.",
  "anya-e1002n0:230_0": "Sakkharāyati merūpi,",
  "anya-e1002n0:231_0": "Visabhakkho sudhāyate;",
  "anya-e1002n0:232_0": "Sasāyate migarāja,",
  "anya-e1002n0:233_0": "Byālo mālāguṇāyate;",
  "anya-e1002n0:234_0": "Dolāyate chamācālo,",
  "anya-e1002n0:235_0": "Nānāvudhā tiṇāyare.",
  "anya-e1002n0:236_0": "21.",
  "anya-e1002n0:237_0": "Sameva sati ussāhe,",
  "anya-e1002n0:238_0": "Sukhavāho hitaṅkaro;",
  "anya-e1002n0:239_0": "Ūne-dhike tathā nohi,",
  "anya-e1002n0:240_0": "Majjhago sādhu sabbadā.",
  "anya-e1002n0:241_0": "Sādhu kho paṇḍitonāma,",
  "anya-e1002n0:242_0": "Natveva atipaṇḍito."
}