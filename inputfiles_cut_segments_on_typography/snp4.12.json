{
  "snp4.12:0.1_0": "Sutta Nipāta 4.12",
  "snp4.12:0.2_0": "Cūḷabyūhasutta",
  "snp4.12:1.1_0": "“Sakaṁsakaṁdiṭṭhiparibbasānā,",
  "snp4.12:1.2_0": "Viggayha nānā kusalā vadanti;",
  "snp4.12:1.3_0": "Yo evaṁ jānāti sa vedi dhammaṁ,",
  "snp4.12:1.4_0": "Idaṁ paṭikkosamakevalī so.",
  "snp4.12:2.1_0": "Evampi viggayha vivādayanti,",
  "snp4.12:2.2_0": "Bālo paro akkusaloti cāhu;",
  "snp4.12:2.3_0": "Sacco nu vādo katamo imesaṁ,",
  "snp4.12:2.4_0": "Sabbeva hīme kusalāvadānā”.",
  "snp4.12:3.1_0": "“Parassa ce dhammamanānujānaṁ,",
  "snp4.12:3.2_0": "Bālomako hoti nihīnapañño;",
  "snp4.12:3.3_0": "Sabbeva bālā sunihīnapaññā,",
  "snp4.12:3.4_0": "Sabbevime diṭṭhiparibbasānā.",
  "snp4.12:4.1_0": "Sandiṭṭhiyā ceva na vīvadātā,",
  "snp4.12:4.2_0": "Saṁsuddhapaññā kusalā mutīmā;",
  "snp4.12:4.3_0": "Na tesaṁ koci parihīnapañño,",
  "snp4.12:4.4_0": "Diṭṭhī hi tesampi tathā samattā.",
  "snp4.12:5.1_0": "Na vāhametaṁ tathiyanti brūmi,",
  "snp4.12:5.2_0": "Yamāhu bālā mithu aññamaññaṁ;",
  "snp4.12:5.3_0": "Sakaṁ sakaṁ diṭṭhimakaṁsu saccaṁ,",
  "snp4.12:5.4_0": "Tasmā hi bāloti paraṁ dahanti”.",
  "snp4.12:6.1_0": "“Yamāhu saccaṁ tathiyanti eke,",
  "snp4.12:6.2_0": "Tamāhu aññe tucchaṁ musāti;",
  "snp4.12:6.3_0": "Evampi vigayha vivādayanti,",
  "snp4.12:6.4_0": "Kasmā na ekaṁ samaṇā vadanti”.",
  "snp4.12:7.1_0": "“Ekañhi saccaṁ na dutīyamatthi,",
  "snp4.12:7.2_0": "Yasmiṁ pajā no vivade pajānaṁ;",
  "snp4.12:7.3_0": "Nānā te saccāni sayaṁ thunanti,",
  "snp4.12:7.4_0": "Tasmā na ekaṁ samaṇā vadanti”.",
  "snp4.12:8.1_0": "“Kasmā nu saccāni vadanti nānā,",
  "snp4.12:8.2_0": "Pavādiyāse kusalāvadānā;",
  "snp4.12:8.3_0": "Saccāni sutāni bahūni nānā,",
  "snp4.12:8.4_0": "Udāhu te takkamanussaranti”.",
  "snp4.12:9.1_0": "“Na heva saccāni bahūni nānā,",
  "snp4.12:9.2_0": "Aññatra saññāya niccāni loke;",
  "snp4.12:9.3_0": "Takkañca diṭṭhīsu pakappayitvā,",
  "snp4.12:9.4_0": "Saccaṁ musāti dvayadhammamāhu.",
  "snp4.12:10.1_0": "Diṭṭhe sute sīlavate mute vā,",
  "snp4.12:10.2_0": "Ete ca nissāya vimānadassī;",
  "snp4.12:10.3_0": "Vinicchaye ṭhatvā pahassamāno,",
  "snp4.12:10.4_0": "Bālo paro akkusaloti cāha.",
  "snp4.12:11.1_0": "Yeneva bāloti paraṁ dahāti,",
  "snp4.12:11.2_0": "Tenātumānaṁ kusaloti cāha;",
  "snp4.12:11.3_0": "Sayamattanā so kusalāvadāno,",
  "snp4.12:11.4_0": "Aññaṁ vimāneti tadeva pāva.",
  "snp4.12:12.1_0": "Atisāradiṭṭhiyā so samatto,",
  "snp4.12:12.2_0": "Mānena matto paripuṇṇamānī;",
  "snp4.12:12.3_0": "Sayameva sāmaṁ manasābhisitto,",
  "snp4.12:12.4_0": "Diṭṭhī hi sā tassa tathā samattā.",
  "snp4.12:13.1_0": "Parassa ce hi vacasā nihīno,",
  "snp4.12:13.2_0": "Tumo sahā hoti nihīnapañño;",
  "snp4.12:13.3_0": "Atha ce sayaṁ vedagū hoti dhīro,",
  "snp4.12:13.4_0": "Na koci bālo samaṇesu atthi.",
  "snp4.12:14.1_0": "Aññaṁ ito yābhivadanti dhammaṁ,",
  "snp4.12:14.2_0": "Aparaddhā suddhimakevalī te;",
  "snp4.12:14.3_0": "Evampi titthyā puthuso vadanti,",
  "snp4.12:14.4_0": "Sandiṭṭhirāgena hi tebhirattā.",
  "snp4.12:15.1_0": "Idheva suddhiṁ iti vādayanti,",
  "snp4.12:15.2_0": "Nāññesu dhammesu visuddhimāhu;",
  "snp4.12:15.3_0": "Evampi titthyā puthuso niviṭṭhā,",
  "snp4.12:15.4_0": "Sakāyane tattha daḷhaṁ vadānā.",
  "snp4.12:16.1_0": "Sakāyane vāpi daḷhaṁ vadāno,",
  "snp4.12:16.2_0": "Kamettha bāloti paraṁ daheyya;",
  "snp4.12:16.3_0": "Sayaṁva so medhagamāvaheyya,",
  "snp4.12:16.4_0": "Paraṁ vadaṁ bālamasuddhidhammaṁ.",
  "snp4.12:17.1_0": "Vinicchaye ṭhatvā sayaṁ pamāya,",
  "snp4.12:17.2_0": "Uddhaṁsa lokasmiṁ vivādameti;",
  "snp4.12:17.3_0": "Hitvāna sabbāni vinicchayāni,",
  "snp4.12:17.4_0": "Na medhagaṁ kubbati jantu loke”ti.",
  "snp4.12:17.5_0": "Cūḷabyūhasuttaṁ dvādasamaṁ."
}