{
  "ja376:0.1_0": "Jātaka",
  "ja376:0.2_0": "Chakkanipāta",
  "ja376:0.3_0": "Avāriyavagga",
  "ja376:0.4_0": "1. Avāriyajātaka",
  "ja376:1.1_0": "“Māsu kujjha bhūmipati,",
  "ja376:1.2_0": "māsu kujjha rathesabha;",
  "ja376:1.3_0": "Kuddhaṁ appaṭikujjhanto,",
  "ja376:1.4_0": "rājā raṭṭhassa pūjito.",
  "ja376:2.1_0": "Gāme vā yadi vāraññe,",
  "ja376:2.2_0": "ninne vā yadi vā thale;",
  "ja376:2.3_0": "Sabbattha anusāsāmi,",
  "ja376:2.4_0": "māsu kujjha rathesabha”.",
  "ja376:3.1_0": "“Avāriyapitā nāma,",
  "ja376:3.2_0": "ahu gaṅgāya nāviko;",
  "ja376:3.3_0": "Pubbe janaṁ tāretvāna,",
  "ja376:3.4_0": "pacchā yācati vetanaṁ;",
  "ja376:3.5_0": "Tenassa bhaṇḍanaṁ hoti,",
  "ja376:3.6_0": "na ca bhogehi vaḍḍhati”.",
  "ja376:4.1_0": "“Atiṇṇaṁyeva yācassu,",
  "ja376:4.2_0": "apāraṁ tāta nāvika;",
  "ja376:4.3_0": "Añño hi tiṇṇassa mano,",
  "ja376:4.4_0": "añño hoti pāresino.",
  "ja376:5.1_0": "Gāme vā yadi vāraññe,",
  "ja376:5.2_0": "ninne vā yadi vā thale;",
  "ja376:5.3_0": "Sabbattha anusāsāmi,",
  "ja376:5.4_0": "māsu kujjhittha nāvika”.",
  "ja376:6.1_0": "“Yāyevānusāsaniyā,",
  "ja376:6.2_0": "rājā gāmavaraṁ adā;",
  "ja376:6.3_0": "Tāyevānusāsaniyā,",
  "ja376:6.4_0": "nāviko paharī mukhaṁ.",
  "ja376:7.1_0": "Bhattaṁ bhinnaṁ hatā bhariyā,",
  "ja376:7.2_0": "Gabbho ca patito chamā;",
  "ja376:7.3_0": "Migova jātarūpena,",
  "ja376:7.4_0": "Na tenatthaṁ abandhi sū”ti.",
  "ja376:8.1_0": "Avāriyajātakaṁ paṭhamaṁ."
}