{
  "thag4.1:0.1_0": "Theragāthā 4.1",
  "thag4.1:0.2_0": "Catukkanipāta",
  "thag4.1:0.3_0": "Paṭhamavagga",
  "thag4.1:0.4_0": "Nāgasamālattheragāthā",
  "thag4.1:1.1_0": "“Alaṅkatā suvasanā,",
  "thag4.1:1.2_0": "mālinī candanussadā;",
  "thag4.1:1.3_0": "Majjhe mahāpathe nārī,",
  "thag4.1:1.4_0": "tūriye naccati naṭṭakī.",
  "thag4.1:2.1_0": "Piṇḍikāya paviṭṭhohaṁ,",
  "thag4.1:2.2_0": "gacchanto naṁ udikkhisaṁ;",
  "thag4.1:2.3_0": "Alaṅkataṁ suvasanaṁ,",
  "thag4.1:2.4_0": "maccupāsaṁva oḍḍitaṁ.",
  "thag4.1:3.1_0": "Tato me manasīkāro,",
  "thag4.1:3.2_0": "yoniso udapajjatha;",
  "thag4.1:3.3_0": "Ādīnavo pāturahu,",
  "thag4.1:3.4_0": "nibbidā samatiṭṭhatha.",
  "thag4.1:4.1_0": "Tato cittaṁ vimucci me,",
  "thag4.1:4.2_0": "passa dhammasudhammataṁ;",
  "thag4.1:4.3_0": "Tisso vijjā anuppattā,",
  "thag4.1:4.4_0": "kataṁ buddhassa sāsanan”ti.",
  "thag4.1:4.5_0": "… Nāgasamālo thero …."
}