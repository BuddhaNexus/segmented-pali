{
  "thig6.3:0.1_0": "Therīgāthā 6.3",
  "thig6.3:0.2_0": "Chakkanipāta",
  "thig6.3:0.3_0": "Khemātherīgāthā",
  "thig6.3:1.1_0": "“Daharā tvaṁ rūpavatī,",
  "thig6.3:1.2_0": "ahampi daharo yuvā;",
  "thig6.3:1.3_0": "Pañcaṅgikena turiyena,",
  "thig6.3:1.4_0": "ehi kheme ramāmase”.",
  "thig6.3:2.1_0": "“Iminā pūtikāyena,",
  "thig6.3:2.2_0": "āturena pabhaṅgunā;",
  "thig6.3:2.3_0": "Aṭṭiyāmi harāyāmi,",
  "thig6.3:2.4_0": "kāmataṇhā samūhatā.",
  "thig6.3:3.1_0": "Sattisūlūpamā kāmā,",
  "thig6.3:3.2_0": "khandhāsaṁ adhikuṭṭanā;",
  "thig6.3:3.3_0": "Yaṁ ‘tvaṁ kāmaratiṁ’ brūsi,",
  "thig6.3:3.4_0": "‘aratī’ dāni sā mama.",
  "thig6.3:4.1_0": "Sabbattha vihatā nandī,",
  "thig6.3:4.2_0": "tamokhandho padālito;",
  "thig6.3:4.3_0": "Evaṁ jānāhi pāpima,",
  "thig6.3:4.4_0": "nihato tvamasi antaka.",
  "thig6.3:5.1_0": "Nakkhattāni namassantā,",
  "thig6.3:5.2_0": "aggiṁ paricaraṁ vane;",
  "thig6.3:5.3_0": "Yathābhuccamajānantā,",
  "thig6.3:5.4_0": "bālā suddhimamaññatha.",
  "thig6.3:6.1_0": "Ahañca kho namassantī,",
  "thig6.3:6.2_0": "sambuddhaṁ purisuttamaṁ;",
  "thig6.3:6.3_0": "Pamuttā sabbadukkhehi,",
  "thig6.3:6.4_0": "satthusāsanakārikā”ti.",
  "thig6.3:7.1_0": "… Khemā therī …."
}