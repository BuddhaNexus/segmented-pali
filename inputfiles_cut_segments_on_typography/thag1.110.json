{
  "thag1.110:0.1_0": "Theragāthā 1.110",
  "thag1.110:0.2_0": "Ekakanipāta",
  "thag1.110:0.3_0": "Ekādasamavagga",
  "thag1.110:0.4_0": "Usabhattheragāthā",
  "thag1.110:1.1_0": "“Nagā nagaggesu susaṁvirūḷhā,",
  "thag1.110:1.2_0": "Udaggameghena navena sittā;",
  "thag1.110:1.3_0": "Vivekakāmassa araññasaññino,",
  "thag1.110:1.4_0": "Janeti bhiyyo usabhassa kalyatan”ti.",
  "thag1.110:1.5_0": "… Usabho thero …",
  "thag1.110:1.6_0": "vaggo ekādasamo.",
  "thag1.110:2.0_0": "Tassuddānaṁ",
  "thag1.110:2.1_0": "Belaṭṭhāniko setuccho,",
  "thag1.110:2.2_0": "Bandhuro khitako isi;",
  "thag1.110:2.3_0": "Malitavambho suhemanto,",
  "thag1.110:2.4_0": "Dhammasavo dhammasavapitā;",
  "thag1.110:2.5_0": "Saṅgharakkhitatthero ca,",
  "thag1.110:2.6_0": "Usabho ca mahāmunīti."
}