{
  "kv13.6:0.1_0": "Kathāvatthu",
  "kv13.6:0.2_0": "Mahāpaṇṇāsaka",
  "kv13.6:0.3_0": "Terasamavagga",
  "kv13.6:0.4_0": "Sammukhībhūtakathā",
  "kv13.6:1.1_0": "Sammukhībhūto saṁyojanaṁ jahatīti?",
  "kv13.6:1.2_0": "Āmantā.",
  "kv13.6:1.3_0": "Ratto rāgaṁ jahati, duṭṭho dosaṁ jahati, mūḷho mohaṁ jahati, kiliṭṭho kilese jahatīti?",
  "kv13.6:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv13.6:1.5_0": "rāgena rāgaṁ jahati, dosena dosaṁ jahati, mohena mohaṁ jahati, kilesehi kilese jahatīti?",
  "kv13.6:1.6_0": "Na hevaṁ vattabbe …pe…",
  "kv13.6:2.1_0": "Rāgo cittasampayutto, maggo cittasampayuttoti?",
  "kv13.6:2.2_0": "Āmantā.",
  "kv13.6:2.3_0": "Dvinnaṁ phassānaṁ …pe…",
  "kv13.6:2.4_0": "dvinnaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv13.6:2.5_0": "Na hevaṁ vattabbe …pe…",
  "kv13.6:2.6_0": "rāgo akusalo, maggo kusaloti?",
  "kv13.6:2.7_0": "Āmantā.",
  "kv13.6:2.8_0": "Kusalākusalā sāvajjānavajjā hīnapaṇītā kaṇhasukkasappaṭibhāgā dhammā sammukhībhāvaṁ āgacchantīti?",
  "kv13.6:2.9_0": "Na hevaṁ vattabbe …pe….",
  "kv13.6:3.1_0": "Kusalākusalā …pe…",
  "kv13.6:3.2_0": "sammukhībhāvaṁ āgacchantīti?",
  "kv13.6:3.3_0": "Āmantā.",
  "kv13.6:3.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv13.6:3.5_0": "“cattārimāni, bhikkhave, suvidūravidūrāni.",
  "kv13.6:3.6_0": "Katamāni cattāri?",
  "kv13.6:3.7_0": "Nabhañca, bhikkhave, pathavī ca—",
  "kv13.6:3.8_0": "idaṁ paṭhamaṁ suvidūravidūraṁ …pe… tasmā sataṁ dhammo asabbhi ārakā”ti.",
  "kv13.6:3.9_0": "Attheva suttantoti?",
  "kv13.6:3.10_0": "Āmantā.",
  "kv13.6:3.11_0": "Tena hi na vattabbaṁ—",
  "kv13.6:3.12_0": "“kusalākusalā …pe…",
  "kv13.6:3.13_0": "sammukhībhāvaṁ āgacchantī”ti.",
  "kv13.6:4.1_0": "Sammukhībhūto saṁyojanaṁ jahatīti?",
  "kv13.6:4.2_0": "Āmantā.",
  "kv13.6:4.3_0": "Nanu vuttaṁ bhagavatā—",
  "kv13.6:4.4_0": "“so evaṁ samāhite citte …pe…",
  "kv13.6:4.5_0": "āsavānaṁ khayañāṇāya cittaṁ abhininnāmetī”ti.",
  "kv13.6:4.6_0": "Attheva suttantoti?",
  "kv13.6:4.7_0": "Āmantā.",
  "kv13.6:4.8_0": "Tena hi na vattabbaṁ—",
  "kv13.6:4.9_0": "“sammukhībhūto saṁyojanaṁ jahatī”ti.",
  "kv13.6:5.1_0": "Na vattabbaṁ—",
  "kv13.6:5.2_0": "“sammukhībhūto saṁyojanaṁ jahatī”ti?",
  "kv13.6:5.3_0": "Āmantā.",
  "kv13.6:5.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv13.6:5.5_0": "“tassa evaṁ jānato evaṁ passato kāmāsavāpi cittaṁ vimuccati …pe…",
  "kv13.6:5.6_0": "avijjāsavāpi cittaṁ vimuccatī”ti.",
  "kv13.6:5.7_0": "Attheva suttantoti?",
  "kv13.6:5.8_0": "Āmantā.",
  "kv13.6:5.9_0": "Tena hi sammukhībhūto saṁyojanaṁ jahatīti.",
  "kv13.6:5.10_0": "Sammukhībhūtakathā niṭṭhitā."
}