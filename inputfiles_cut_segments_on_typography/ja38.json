{
  "ja38:0.1_0": "Jātaka",
  "ja38:0.2_0": "Ekakanipāta",
  "ja38:0.3_0": "Kulāvakavagga",
  "ja38:0.4_0": "8. Bakajātaka",
  "ja38:1.1_0": "“Nāccantaṁ nikatippañño,",
  "ja38:1.2_0": "nikatyā sukhamedhati;",
  "ja38:1.3_0": "Ārādheti nikatippañño,",
  "ja38:1.4_0": "bako kakkaṭakāmivā”ti.",
  "ja38:2.1_0": "Bakajātakaṁ aṭṭhamaṁ."
}