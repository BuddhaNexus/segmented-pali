{
  "tha-ap443:0.1_0": "Therāpadāna",
  "tha-ap443:0.2_0": "Vibhītakavagga",
  "tha-ap443:0.3_0": "1 Vibhītakamiñjiyattheraapadāna",
  "tha-ap443:1.1_0": "“Kakusandho mahāvīro,",
  "tha-ap443:1.2_0": "sabbadhammāna pāragū;",
  "tha-ap443:1.3_0": "Gaṇamhā vūpakaṭṭho so,",
  "tha-ap443:1.4_0": "agamāsi vanantaraṁ.",
  "tha-ap443:2.1_0": "Bījamiñjaṁ gahetvāna,",
  "tha-ap443:2.2_0": "latāya āvuṇiṁ ahaṁ;",
  "tha-ap443:2.3_0": "Bhagavā tamhi samaye,",
  "tha-ap443:2.4_0": "jhāyate pabbatantare.",
  "tha-ap443:3.1_0": "Disvānahaṁ devadevaṁ,",
  "tha-ap443:3.2_0": "vippasannena cetasā;",
  "tha-ap443:3.3_0": "Dakkhiṇeyyassa vīrassa,",
  "tha-ap443:3.4_0": "bījamiñjamadāsahaṁ.",
  "tha-ap443:4.1_0": "Imasmiṁyeva kappamhi,",
  "tha-ap443:4.2_0": "yaṁ miñjamadadiṁ tadā;",
  "tha-ap443:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap443:4.4_0": "bījamiñjassidaṁ phalaṁ.",
  "tha-ap443:5.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap443:5.2_0": "bhavā sabbe samūhatā;",
  "tha-ap443:5.3_0": "Nāgova bandhanaṁ chetvā,",
  "tha-ap443:5.4_0": "viharāmi anāsavo.",
  "tha-ap443:6.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap443:6.2_0": "mama buddhassa santike;",
  "tha-ap443:6.3_0": "Tisso vijjā anuppattā,",
  "tha-ap443:6.4_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap443:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap443:7.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap443:7.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap443:7.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap443:8.1_0": "Itthaṁ sudaṁ āyasmā vibhītakamiñjiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap443:9.1_0": "Vibhītakamiñjiyattherassāpadānaṁ paṭhamaṁ."
}