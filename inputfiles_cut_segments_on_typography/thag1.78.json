{
  "thag1.78:0.1_0": "Theragāthā 1.78",
  "thag1.78:0.2_0": "Ekakanipāta",
  "thag1.78:0.3_0": "Aṭṭhamavagga",
  "thag1.78:0.4_0": "Meṇḍasirattheragāthā",
  "thag1.78:1.1_0": "“Anekajātisaṁsāraṁ,",
  "thag1.78:1.2_0": "sandhāvissaṁ anibbisaṁ;",
  "thag1.78:1.3_0": "Tassa me dukkhajātassa,",
  "thag1.78:1.4_0": "dukkhakkhandho aparaddho”ti.",
  "thag1.78:1.5_0": "… Meṇḍasiro thero …."
}