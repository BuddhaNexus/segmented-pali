{
  "anya-e1006n:0_0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1006n:1_0": "Suttantanīti",
  "anya-e1006n:2_0": "1.",
  "anya-e1006n:3_0": "Parā bhavantaṃ purisaṃ,",
  "anya-e1006n:4_0": "Mayaṃ pucchāma gotamaṃ;",
  "anya-e1006n:5_0": "Bhavantaṃ puṭṭhu māgamma,",
  "anya-e1006n:6_0": "Kiṃ parābhavato mukhaṃ.",
  "anya-e1006n:7_0": "2.",
  "anya-e1006n:8_0": "Suvijāno bhavaṃhoti,",
  "anya-e1006n:9_0": "Duvijāno parābhavo;",
  "anya-e1006n:10_0": "Dhammakāmo bhavaṃ hoti,",
  "anya-e1006n:11_0": "Dhammadessī parābhavo.",
  "anya-e1006n:12_0": "3.",
  "anya-e1006n:13_0": "Itihe taṃ vijānāma,",
  "anya-e1006n:14_0": "Pathamo so parābhavo;",
  "anya-e1006n:15_0": "Dutīyaṃ bhagavā brūhi,",
  "anya-e1006n:16_0": "Kiṃ parābhavato mukhaṃ.",
  "anya-e1006n:17_0": "4.",
  "anya-e1006n:18_0": "Asantassa piyo hoti,",
  "anya-e1006n:19_0": "Sante na kurute piyaṃ;",
  "anya-e1006n:20_0": "Asataṃ dhammaṃ roceti,",
  "anya-e1006n:21_0": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:22_0": "Kammāparādhasattānaṃ,",
  "anya-e1006n:23_0": "Vināse paccupaṭṭhite;",
  "anya-e1006n:24_0": "Anayo nayarūpena,",
  "anya-e1006n:25_0": "Buddhimākamya tiṭṭhati.",
  "anya-e1006n:26_0": "5.",
  "anya-e1006n:27_0": "Niddāsīlī sabhāsīlī,",
  "anya-e1006n:28_0": "Anuṭṭhātā ca yo naro;",
  "anya-e1006n:29_0": "Alaso kodhapaññāṇo,",
  "anya-e1006n:30_0": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:31_0": "6.",
  "anya-e1006n:32_0": "Yo mātaraṃ pitaraṃ vā,",
  "anya-e1006n:33_0": "Jiṇṇakaṃ gatayobbanaṃ;",
  "anya-e1006n:34_0": "Pahusanto na bharati,",
  "anya-e1006n:35_0": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:36_0": "7.",
  "anya-e1006n:37_0": "Yo brāhmaṇaṃ samaṇaṃ vā,",
  "anya-e1006n:38_0": "Aññaṃ vāpi vaṇibbakaṃ;",
  "anya-e1006n:39_0": "Musāvādena vañceti,",
  "anya-e1006n:40_0": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:41_0": "8.",
  "anya-e1006n:42_0": "Pahutavitto puriso,",
  "anya-e1006n:43_0": "Sahirañño sabhojano;",
  "anya-e1006n:44_0": "Eko bhuñjati sādūni,",
  "anya-e1006n:45_0": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:46_0": "9.",
  "anya-e1006n:47_0": "Jātithaddho dhanathaddho,",
  "anya-e1006n:48_0": "Gottathaddho ca yo naro;",
  "anya-e1006n:49_0": "Sañātiṃ atimaññeti,",
  "anya-e1006n:50_0": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:51_0": "10.",
  "anya-e1006n:52_0": "Itthidhutto surādhutto,",
  "anya-e1006n:53_0": "Akkhadhutto ca yo naro;",
  "anya-e1006n:54_0": "Laddhaṃladdhaṃ vināseti,",
  "anya-e1006n:55_0": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:56_0": "Na bāḷhaṃ itthiṃ gaccheyya,",
  "anya-e1006n:57_0": "Sampassaṃ tejasaṅkhayaṃ;",
  "anya-e1006n:58_0": "Kāsaṃ sāsaṃ daraṃ bālyaṃ,",
  "anya-e1006n:59_0": "Khīṇamedo nigacchati.",
  "anya-e1006n:60_0": "(Ka)",
  "anya-e1006n:61_0": "Māyācetā marīcī ca,",
  "anya-e1006n:62_0": "Soko rogo upaddavo;",
  "anya-e1006n:63_0": "Kharā ca bandhanācetā,",
  "anya-e1006n:64_0": "Maccupāso guhāsayo.",
  "anya-e1006n:65_0": "(Kha)",
  "anya-e1006n:66_0": "Balavanto dubbalā honti,",
  "anya-e1006n:67_0": "Thāmavantopi hāyare;",
  "anya-e1006n:68_0": "Cakkhumā andhakā honti,",
  "anya-e1006n:69_0": "Mātugāmavasaṃgatā.",
  "anya-e1006n:70_0": "(Ga)",
  "anya-e1006n:71_0": "Guṇavanto nigguṇā honti,",
  "anya-e1006n:72_0": "Paññavantopi hāyare;",
  "anya-e1006n:73_0": "Pamuttā bandhanā senti,",
  "anya-e1006n:74_0": "Mātugāmavasaṃgatā.",
  "anya-e1006n:75_0": "(Gha)",
  "anya-e1006n:76_0": "Yasaṃ kittiṃ dhitiṃ sūraṃ;",
  "anya-e1006n:77_0": "Bāhussaccaṃ pajānanaṃ;",
  "anya-e1006n:78_0": "Hāpayanti pamattassa;",
  "anya-e1006n:79_0": "Kaṭṭhapuñcaṃva pāvako.",
  "anya-e1006n:80_0": "11.",
  "anya-e1006n:81_0": "Sehi dārehi santuṭṭho,",
  "anya-e1006n:82_0": "Vesiyāsu padussati;",
  "anya-e1006n:83_0": "Dussati paradāresu,",
  "anya-e1006n:84_0": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:85_0": "(Ka)",
  "anya-e1006n:86_0": "Mayañca bhariyaṃ nātikkamāma,",
  "anya-e1006n:87_0": "Amheca bhariyā nātikkamanti;",
  "anya-e1006n:88_0": "Aññatra tāhi brahmacariyaṃ carāma,",
  "anya-e1006n:89_0": "Tasmā hi amhaṃ daharā na miyyare.",
  "anya-e1006n:90_0": "(Kha)",
  "anya-e1006n:91_0": "Etāsu jāyare suttamāsu,",
  "anya-e1006n:92_0": "Medhāvino honti pahutapaññā;",
  "anya-e1006n:93_0": "Bahussutā theraguṇā ca honti,",
  "anya-e1006n:94_0": "Tasmā hi amhaṃ daharā na miyyare.",
  "anya-e1006n:95_0": "12.",
  "anya-e1006n:96_0": "Atītayobbano poso,",
  "anya-e1006n:97_0": "Āneti timbarutthaniṃ;",
  "anya-e1006n:98_0": "Tassa issā na supati,",
  "anya-e1006n:99_0": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:100_0": "(Ka)",
  "anya-e1006n:101_0": "Na dukkhaṃ ahinā daṭṭhaṃ,",
  "anya-e1006n:102_0": "Na dukkhaṃ sattiyā hataṃ;",
  "anya-e1006n:103_0": "Tañca dukkhañca tibbañca,",
  "anya-e1006n:104_0": "Yaṃ passe jiṇṇakaṃ patiṃ.",
  "anya-e1006n:105_0": "(Kha)",
  "anya-e1006n:106_0": "Natthi khiṭṭā natthi rati,",
  "anya-e1006n:107_0": "Jiṇṇena patinā saha;",
  "anya-e1006n:108_0": "Natthi allāpasallāpo,",
  "anya-e1006n:109_0": "Jagghituṃpi na sobhati.",
  "anya-e1006n:110_0": "(Ga)",
  "anya-e1006n:111_0": "Yadā ca daharo daharī,",
  "anya-e1006n:112_0": "Mantayiṃsu rahogatā;",
  "anya-e1006n:113_0": "Sabbe sokā vinassanti,",
  "anya-e1006n:114_0": "Yekeci hadayassitā.",
  "anya-e1006n:115_0": "13.",
  "anya-e1006n:116_0": "Itthiṃ soṇḍiṃ vikiraṇiṃ,",
  "anya-e1006n:117_0": "Purisaṃ vāpi tādisaṃ;",
  "anya-e1006n:118_0": "Issariyasmiṃ thapeti,",
  "anya-e1006n:119_0": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:120_0": "14.",
  "anya-e1006n:121_0": "Appabhogo mahātaṇho,",
  "anya-e1006n:122_0": "Khattiye jāyate kule;",
  "anya-e1006n:123_0": "So ca rajjaṃ patthayati,",
  "anya-e1006n:124_0": "Taṃ parābhavato mukhaṃ.",
  "anya-e1006n:125_0": "“Suvijāno bhavaṃhoti,",
  "anya-e1006n:126_0": "Duvijāno parābhavo”;",
  "anya-e1006n:127_0": "Appabhogo mahātaṇho.",
  "anya-e1006n:128_0": "15.",
  "anya-e1006n:129_0": "Ete parābhave loke,",
  "anya-e1006n:130_0": "Paṇḍito samavekkhiya;",
  "anya-e1006n:131_0": "Ariyo dassanasampanno,",
  "anya-e1006n:132_0": "Sa lokaṃ bhajate sivaṃ.",
  "anya-e1006n:133_0": "Vasalasutta",
  "anya-e1006n:134_0": "1.",
  "anya-e1006n:135_0": "Kodhano upanāhīca,",
  "anya-e1006n:136_0": "Pāpamakkhī ca yo naro;",
  "anya-e1006n:137_0": "Vipannadiṭṭhī māyāvī,",
  "anya-e1006n:138_0": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:139_0": "2.",
  "anya-e1006n:140_0": "Ekajaṃ vā dvijaṃ vāpi,",
  "anya-e1006n:141_0": "Yodha pāṇaṃ vihiṃsati;",
  "anya-e1006n:142_0": "Yassa pāṇe dayā natthi,",
  "anya-e1006n:143_0": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:144_0": "3.",
  "anya-e1006n:145_0": "Yo hanti uparundhati,",
  "anya-e1006n:146_0": "Gāmāni nigamāni ca;",
  "anya-e1006n:147_0": "Niggāhako samaññāto,",
  "anya-e1006n:148_0": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:149_0": "4.",
  "anya-e1006n:150_0": "Gāme vā yadi vā raññe,",
  "anya-e1006n:151_0": "Yaṃ paresaṃ mamāyitaṃ;",
  "anya-e1006n:152_0": "Theyyā adinnaṃ ādeti,",
  "anya-e1006n:153_0": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:154_0": "5.",
  "anya-e1006n:155_0": "Yo have iṇamādāya,",
  "anya-e1006n:156_0": "Vuccamāno palāyati;",
  "anya-e1006n:157_0": "Na hi te iṇamatthīti,",
  "anya-e1006n:158_0": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:159_0": "6.",
  "anya-e1006n:160_0": "Yodha kiñcikkhakamyatā,",
  "anya-e1006n:161_0": "Pathasmiṃ vajataṃ janaṃ;",
  "anya-e1006n:162_0": "Hantvā kiñcikkha mādeti;",
  "anya-e1006n:163_0": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:164_0": "7.",
  "anya-e1006n:165_0": "Yo attahetu parahetu,",
  "anya-e1006n:166_0": "Dhanahetu ca yo naro;",
  "anya-e1006n:167_0": "Sakkhipuṭṭho musābrūti,",
  "anya-e1006n:168_0": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:169_0": "8.",
  "anya-e1006n:170_0": "Yo ñātīnaṃ sakhīnaṃ vā,",
  "anya-e1006n:171_0": "Dāresu paṭidissati;",
  "anya-e1006n:172_0": "Sahasā sampiyena vā,",
  "anya-e1006n:173_0": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:174_0": "9.",
  "anya-e1006n:175_0": "Yo 4 mātaraṃ pitaraṃ vā,",
  "anya-e1006n:176_0": "Jiṇṇakaṃ gatayobbanaṃ;",
  "anya-e1006n:177_0": "Pahusanto na bharati,",
  "anya-e1006n:178_0": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:179_0": "10.",
  "anya-e1006n:180_0": "Yo mātaraṃ pitaraṃ vā,",
  "anya-e1006n:181_0": "Bhātaraṃ bhaginiṃ sassuṃ;",
  "anya-e1006n:182_0": "Hanti roseti vācāya,",
  "anya-e1006n:183_0": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:184_0": "11.",
  "anya-e1006n:185_0": "Yo atthaṃ pucchito santo,",
  "anya-e1006n:186_0": "Anattha manusāsati;",
  "anya-e1006n:187_0": "Paṭicchannena manteti,",
  "anya-e1006n:188_0": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:189_0": "12.",
  "anya-e1006n:190_0": "Yo katvā pāpakaṃ kammaṃ,",
  "anya-e1006n:191_0": "Māmaṃ jaññāti icchati;",
  "anya-e1006n:192_0": "Yo paṭicchannakammanto,",
  "anya-e1006n:193_0": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:194_0": "13.",
  "anya-e1006n:195_0": "Yo ve parakulaṃ gantvā,",
  "anya-e1006n:196_0": "Bhutvāna sucibhojanaṃ;",
  "anya-e1006n:197_0": "Āgataṃ nappaṭipūjeti,",
  "anya-e1006n:198_0": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:199_0": "14.",
  "anya-e1006n:200_0": "Yo samaṇaṃ vā brāhmaṇaṃ,",
  "anya-e1006n:201_0": "Aññaṃ vāpi vaṇibbakaṃ;",
  "anya-e1006n:202_0": "Musāvādena vañceti,",
  "anya-e1006n:203_0": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:204_0": "15.",
  "anya-e1006n:205_0": "Yo samaṇaṃ vā brāhmaṇaṃ,",
  "anya-e1006n:206_0": "Bhattakāle upaṭṭhitaṃ;",
  "anya-e1006n:207_0": "Roseti vā na ca deti,",
  "anya-e1006n:208_0": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:209_0": "16.",
  "anya-e1006n:210_0": "Asantaṃ yodha pabrūti,",
  "anya-e1006n:211_0": "Mohena paliguṇṭhito;",
  "anya-e1006n:212_0": "Kiñcanaṃ nijigīsāno,",
  "anya-e1006n:213_0": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:214_0": "17.",
  "anya-e1006n:215_0": "Yocattānaṃ samukkaṃse,",
  "anya-e1006n:216_0": "Pareca mavajānāti;",
  "anya-e1006n:217_0": "Nihīno sena mānena,",
  "anya-e1006n:218_0": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:219_0": "18.",
  "anya-e1006n:220_0": "Rosako kadarīyo ca,",
  "anya-e1006n:221_0": "Pāpiccho maccharī saṭho;",
  "anya-e1006n:222_0": "Ahirīko anottappī,",
  "anya-e1006n:223_0": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:224_0": "19.",
  "anya-e1006n:225_0": "Yo buddhaṃ paribhāsati,",
  "anya-e1006n:226_0": "Athavā tassa sāvakaṃ;",
  "anya-e1006n:227_0": "Paribbajaṃ gahaṭṭhaṃ vā,",
  "anya-e1006n:228_0": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:229_0": "Aṭṭhahi bhikkhave aṅgehi sampannāgatassa upāsakassa patto nikujjitabbo.",
  "anya-e1006n:229_1": "Bhikkhūnaṃ alābhāya parisakkati,",
  "anya-e1006n:229_2": "bhikkhūnaṃ anatthāya parisakkati,",
  "anya-e1006n:229_3": "bhikkhūnaṃ anāvāsāya parisakkati,",
  "anya-e1006n:229_4": "bhikkhūnaṃ akkosati paribhāsati,",
  "anya-e1006n:229_5": "bhikkhūbhikkhūhi bhedeti,",
  "anya-e1006n:229_6": "buddhassa avaṇṇaṃ bhāsati,",
  "anya-e1006n:229_7": "dhammassa avaṇṇaṃ bhāsati,",
  "anya-e1006n:229_8": "saṅghassa avaṇṇaṃ bhāsati,",
  "anya-e1006n:229_9": "anujānāmi bhikkhave imehi aṭṭhahi aṅgehi sampannāgatassa upāsakassa pattaṃ nikujjituṃ.",
  "anya-e1006n:230_0": "20.",
  "anya-e1006n:231_0": "Yo anarahaṃ santo,",
  "anya-e1006n:232_0": "Arahāti paṭijānāti;",
  "anya-e1006n:233_0": "Coro sabrahmake loke,",
  "anya-e1006n:234_0": "Taṃ jaññā vasalo iti.",
  "anya-e1006n:235_0": "21.",
  "anya-e1006n:236_0": "Ete kho vasalā vuttā,",
  "anya-e1006n:237_0": "Mayā ye te pakāsitā;",
  "anya-e1006n:238_0": "Na jaccā vasalo hoti,",
  "anya-e1006n:239_0": "Na jaccā hoti brāhmaṇo.",
  "anya-e1006n:240_0": "22.",
  "anya-e1006n:241_0": "Kammunā vasalo hoti,",
  "anya-e1006n:242_0": "Kammunā hoti brāhmaṇo.",
  "anya-e1006n:243_0": "23.",
  "anya-e1006n:244_0": "Tadamināpi jānātha,",
  "anya-e1006n:245_0": "Yathāhetaṃ nidassanaṃ;",
  "anya-e1006n:246_0": "Caṇḍālaputto sopāko,",
  "anya-e1006n:247_0": "Mātaṅgo iti vissuto.",
  "anya-e1006n:248_0": "24.",
  "anya-e1006n:249_0": "So yasaṃ paramaṃ patto,",
  "anya-e1006n:250_0": "Mātaṅgo yaṃ sudullabhaṃ;",
  "anya-e1006n:251_0": "Āgacchuṃ tassupaṭṭhānaṃ,",
  "anya-e1006n:252_0": "Khattiyā brāhmaṇā bahū.",
  "anya-e1006n:253_0": "25.",
  "anya-e1006n:254_0": "So devayānaṃ abhiruyha,",
  "anya-e1006n:255_0": "Virajaṃ so mahāpathaṃ;",
  "anya-e1006n:256_0": "Kāmarāgaṃ virājetvā,",
  "anya-e1006n:257_0": "Brahmalokūpago ahu.",
  "anya-e1006n:258_0": "Giriṃ nakhena khaṇasi,",
  "anya-e1006n:259_0": "Ayo dantebhi khādasi;",
  "anya-e1006n:260_0": "Jātavedaṃ padahasi,",
  "anya-e1006n:261_0": "Yo isiṃ paribhāsati.",
  "anya-e1006n:262_0": "Āvelitaṃ piṭṭhito uttamaṅgaṃ,",
  "anya-e1006n:263_0": "Bāhuṃ pasāreti akampaṇeyyaṃ;",
  "anya-e1006n:264_0": "Setāni akkhīni yathā matassa,",
  "anya-e1006n:265_0": "Ko me imaṃ puttamakāsi evaṃ."
}