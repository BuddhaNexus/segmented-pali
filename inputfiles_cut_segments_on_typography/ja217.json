{
  "ja217:0.1_0": "Jātaka",
  "ja217:0.2_0": "Dukanipāta",
  "ja217:0.3_0": "Bīraṇathambhavagga",
  "ja217:0.4_0": "7. Seggujātaka",
  "ja217:1.1_0": "“Sabbo loko attamano ahosi,",
  "ja217:1.2_0": "Akovidā gāmadhammassa seggu;",
  "ja217:1.3_0": "Komāri ko nāma tavajja dhammo,",
  "ja217:1.4_0": "Yaṁ tvaṁ gahitā pavane parodasi”.",
  "ja217:2.1_0": "“Yo dukkhaphuṭṭhāya bhaveyya tāṇaṁ,",
  "ja217:2.2_0": "So me pitā dubbhi vane karoti;",
  "ja217:2.3_0": "Sā kassa kandāmi vanassa majjhe,",
  "ja217:2.4_0": "Yo tāyitā so sahasaṁ karotī”ti.",
  "ja217:3.1_0": "Seggujātakaṁ sattamaṁ."
}