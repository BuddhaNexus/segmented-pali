{
  "thi-ap2:0.1_0": "Therīapadāna",
  "thi-ap2:0.2_0": "Sumedhāvagga",
  "thi-ap2:0.3_0": "2 Mekhalādāyikātherīapadāna",
  "thi-ap2:1.1_0": "“Siddhatthassa bhagavato,",
  "thi-ap2:1.2_0": "thūpakārāpikā ahuṁ;",
  "thi-ap2:1.3_0": "Mekhalikā mayā dinnā,",
  "thi-ap2:1.4_0": "navakammāya satthuno.",
  "thi-ap2:2.1_0": "Niṭṭhite ca mahāthūpe,",
  "thi-ap2:2.2_0": "mekhalaṁ punadāsahaṁ;",
  "thi-ap2:2.3_0": "Lokanāthassa munino,",
  "thi-ap2:2.4_0": "pasannā sehi pāṇibhi.",
  "thi-ap2:3.1_0": "Catunnavutito kappe,",
  "thi-ap2:3.2_0": "yaṁ mekhalamadaṁ tadā;",
  "thi-ap2:3.3_0": "Duggatiṁ nābhijānāmi,",
  "thi-ap2:3.4_0": "thūpakārassidaṁ phalaṁ.",
  "thi-ap2:4.1_0": "Kilesā jhāpitā mayhaṁ,",
  "thi-ap2:4.2_0": "…pe…",
  "thi-ap2:4.3_0": "viharāmi anāsavā.",
  "thi-ap2:5.1_0": "Svāgataṁ vata me āsi,",
  "thi-ap2:5.2_0": "…pe…",
  "thi-ap2:5.3_0": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap2:6.1_0": "Paṭisambhidā catasso,",
  "thi-ap2:6.2_0": "…pe…",
  "thi-ap2:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "thi-ap2:7.1_0": "Itthaṁ sudaṁ mekhalādāyikā bhikkhunī imā gāthāyo abhāsitthāti.",
  "thi-ap2:8.1_0": "Mekhalādāyikātheriyāpadānaṁ dutiyaṁ."
}