{
  "thig13.5:0.1_0": "Therīgāthā 13.5",
  "thig13.5:0.2_0": "Vīsatinipāta",
  "thig13.5:0.3_0": "Subhākammāradhītutherīgāthā",
  "thig13.5:1.1_0": "“Daharāhaṁ suddhavasanā,",
  "thig13.5:1.2_0": "yaṁ pure dhammamassuṇiṁ;",
  "thig13.5:1.3_0": "Tassā me appamattāya,",
  "thig13.5:1.4_0": "saccābhisamayo ahu.",
  "thig13.5:2.1_0": "Tatohaṁ sabbakāmesu,",
  "thig13.5:2.2_0": "bhusaṁ aratimajjhagaṁ;",
  "thig13.5:2.3_0": "Sakkāyasmiṁ bhayaṁ disvā,",
  "thig13.5:2.4_0": "nekkhammameva pīhaye.",
  "thig13.5:3.1_0": "Hitvānahaṁ ñātigaṇaṁ,",
  "thig13.5:3.2_0": "dāsakammakarāni ca;",
  "thig13.5:3.3_0": "Gāmakhettāni phītāni,",
  "thig13.5:3.4_0": "ramaṇīye pamodite.",
  "thig13.5:4.1_0": "Pahāyahaṁ pabbajitā,",
  "thig13.5:4.2_0": "sāpateyyamanappakaṁ;",
  "thig13.5:4.3_0": "Evaṁ saddhāya nikkhamma,",
  "thig13.5:4.4_0": "saddhamme suppavedite.",
  "thig13.5:5.1_0": "Netaṁ assa patirūpaṁ,",
  "thig13.5:5.2_0": "Ākiñcaññañhi patthaye;",
  "thig13.5:5.3_0": "Yo jātarūpaṁ rajataṁ,",
  "thig13.5:5.4_0": "Chaḍḍetvā punarāgame.",
  "thig13.5:6.1_0": "Rajataṁ jātarūpaṁ vā,",
  "thig13.5:6.2_0": "na bodhāya na santiyā;",
  "thig13.5:6.3_0": "Netaṁ samaṇasāruppaṁ,",
  "thig13.5:6.4_0": "na etaṁ ariyaddhanaṁ.",
  "thig13.5:7.1_0": "Lobhanaṁ madanañcetaṁ,",
  "thig13.5:7.2_0": "mohanaṁ rajavaḍḍhanaṁ;",
  "thig13.5:7.3_0": "Sāsaṅkaṁ bahuāyāsaṁ,",
  "thig13.5:7.4_0": "natthi cettha dhuvaṁ ṭhiti.",
  "thig13.5:8.1_0": "Ettha rattā pamattā ca,",
  "thig13.5:8.2_0": "saṅkiliṭṭhamanā narā;",
  "thig13.5:8.3_0": "Aññamaññena byāruddhā,",
  "thig13.5:8.4_0": "puthu kubbanti medhagaṁ.",
  "thig13.5:9.1_0": "Vadho bandho parikleso,",
  "thig13.5:9.2_0": "jāni sokapariddavo;",
  "thig13.5:9.3_0": "Kāmesu adhipannānaṁ,",
  "thig13.5:9.4_0": "dissate byasanaṁ bahuṁ.",
  "thig13.5:10.1_0": "Taṁ maṁ ñātī amittāva,",
  "thig13.5:10.2_0": "kiṁ vo kāmesu yuñjatha;",
  "thig13.5:10.3_0": "Jānātha maṁ pabbajitaṁ,",
  "thig13.5:10.4_0": "kāmesu bhayadassiniṁ.",
  "thig13.5:11.1_0": "Na hiraññasuvaṇṇena,",
  "thig13.5:11.2_0": "parikkhīyanti āsavā;",
  "thig13.5:11.3_0": "Amittā vadhakā kāmā,",
  "thig13.5:11.4_0": "sapattā sallabandhanā.",
  "thig13.5:12.1_0": "Taṁ maṁ ñātī amittāva,",
  "thig13.5:12.2_0": "kiṁ vo kāmesu yuñjatha;",
  "thig13.5:12.3_0": "Jānātha maṁ pabbajitaṁ,",
  "thig13.5:12.4_0": "muṇḍaṁ saṅghāṭipārutaṁ.",
  "thig13.5:13.1_0": "Uttiṭṭhapiṇḍo uñcho ca,",
  "thig13.5:13.2_0": "paṁsukūlañca cīvaraṁ;",
  "thig13.5:13.3_0": "Etaṁ kho mama sāruppaṁ,",
  "thig13.5:13.4_0": "anagārūpanissayo.",
  "thig13.5:14.1_0": "Vantā mahesīhi kāmā,",
  "thig13.5:14.2_0": "ye dibbā ye ca mānusā;",
  "thig13.5:14.3_0": "Khemaṭṭhāne vimuttā te,",
  "thig13.5:14.4_0": "pattā te acalaṁ sukhaṁ.",
  "thig13.5:15.1_0": "Māhaṁ kāmehi saṅgacchiṁ,",
  "thig13.5:15.2_0": "yesu tāṇaṁ na vijjati;",
  "thig13.5:15.3_0": "Amittā vadhakā kāmā,",
  "thig13.5:15.4_0": "aggikkhandhūpamā dukhā.",
  "thig13.5:16.1_0": "Paripantho esa bhayo,",
  "thig13.5:16.2_0": "savighāto sakaṇṭako;",
  "thig13.5:16.3_0": "Gedho suvisamo ceso,",
  "thig13.5:16.4_0": "mahanto mohanāmukho.",
  "thig13.5:17.1_0": "Upasaggo bhīmarūpo,",
  "thig13.5:17.2_0": "kāmā sappasirūpamā;",
  "thig13.5:17.3_0": "Ye bālā abhinandanti,",
  "thig13.5:17.4_0": "andhabhūtā puthujjanā.",
  "thig13.5:18.1_0": "Kāmapaṅkena sattā hi,",
  "thig13.5:18.2_0": "bahū loke aviddasū;",
  "thig13.5:18.3_0": "Pariyantaṁ na jānanti,",
  "thig13.5:18.4_0": "jātiyā maraṇassa ca.",
  "thig13.5:19.1_0": "Duggatigamanaṁ maggaṁ,",
  "thig13.5:19.2_0": "manussā kāmahetukaṁ;",
  "thig13.5:19.3_0": "Bahuṁ ve paṭipajjanti,",
  "thig13.5:19.4_0": "attano rogamāvahaṁ.",
  "thig13.5:20.1_0": "Evaṁ amittajananā,",
  "thig13.5:20.2_0": "tāpanā saṅkilesikā;",
  "thig13.5:20.3_0": "Lokāmisā bandhanīyā,",
  "thig13.5:20.4_0": "kāmā maraṇabandhanā.",
  "thig13.5:21.1_0": "Ummādanā ullapanā,",
  "thig13.5:21.2_0": "kāmā cittappamaddino;",
  "thig13.5:21.3_0": "Sattānaṁ saṅkilesāya,",
  "thig13.5:21.4_0": "khippaṁ mārena oḍḍitaṁ.",
  "thig13.5:22.1_0": "Anantādīnavā kāmā,",
  "thig13.5:22.2_0": "bahudukkhā mahāvisā;",
  "thig13.5:22.3_0": "Appassādā raṇakarā,",
  "thig13.5:22.4_0": "sukkapakkhavisosanā.",
  "thig13.5:23.1_0": "Sāhaṁ etādisaṁ katvā,",
  "thig13.5:23.2_0": "byasanaṁ kāmahetukaṁ;",
  "thig13.5:23.3_0": "Na taṁ paccāgamissāmi,",
  "thig13.5:23.4_0": "nibbānābhiratā sadā.",
  "thig13.5:24.1_0": "Raṇaṁ karitvā kāmānaṁ,",
  "thig13.5:24.2_0": "sītibhāvābhikaṅkhinī;",
  "thig13.5:24.3_0": "Appamattā vihassāmi,",
  "thig13.5:24.4_0": "sabbasaṁyojanakkhaye.",
  "thig13.5:25.1_0": "Asokaṁ virajaṁ khemaṁ,",
  "thig13.5:25.2_0": "ariyaṭṭhaṅgikaṁ ujuṁ;",
  "thig13.5:25.3_0": "Taṁ maggaṁ anugacchāmi,",
  "thig13.5:25.4_0": "yena tiṇṇā mahesino”.",
  "thig13.5:26.1_0": "“Imaṁ passatha dhammaṭṭhaṁ,",
  "thig13.5:26.2_0": "subhaṁ kammāradhītaraṁ;",
  "thig13.5:26.3_0": "Anejaṁ upasampajja,",
  "thig13.5:26.4_0": "rukkhamūlamhi jhāyati.",
  "thig13.5:27.1_0": "Ajjaṭṭhamī pabbajitā,",
  "thig13.5:27.2_0": "saddhā saddhammasobhanā;",
  "thig13.5:27.3_0": "Vinītuppalavaṇṇāya,",
  "thig13.5:27.4_0": "tevijjā maccuhāyinī.",
  "thig13.5:28.1_0": "Sāyaṁ bhujissā anaṇā,",
  "thig13.5:28.2_0": "bhikkhunī bhāvitindriyā;",
  "thig13.5:28.3_0": "Sabbayogavisaṁyuttā,",
  "thig13.5:28.4_0": "katakiccā anāsavā”.",
  "thig13.5:29.1_0": "Taṁ sakko devasaṅghena,",
  "thig13.5:29.2_0": "upasaṅkamma iddhiyā;",
  "thig13.5:29.3_0": "Namassati bhūtapati,",
  "thig13.5:29.4_0": "subhaṁ kammāradhītaranti.",
  "thig13.5:30.1_0": "… Subhā kammāradhītā therī ….",
  "thig13.5:31.1_0": "Vīsatinipāto niṭṭhito."
}