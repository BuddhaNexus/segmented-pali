{
  "tha-ap308:0.1_0": "Therāpadāna",
  "tha-ap308:0.2_0": "Padumakesaravagga",
  "tha-ap308:0.3_0": "6 Sampasādakattheraapadāna",
  "tha-ap308:1.1_0": "“‘Namo te buddha vīratthu,",
  "tha-ap308:1.2_0": "vippamuttosi sabbadhi;",
  "tha-ap308:1.3_0": "Byasanamhi anuppatto,",
  "tha-ap308:1.4_0": "tassa me saraṇaṁ bhava’.",
  "tha-ap308:2.1_0": "Siddhattho tassa byākāsi,",
  "tha-ap308:2.2_0": "loke appaṭipuggalo;",
  "tha-ap308:2.3_0": "‘Mahodadhisamo saṅgho,",
  "tha-ap308:2.4_0": "appameyyo anuttaro.",
  "tha-ap308:3.1_0": "Tattha tvaṁ viraje khette,",
  "tha-ap308:3.2_0": "anantaphaladāyake;",
  "tha-ap308:3.3_0": "Saṅghe cittaṁ pasādetvā,",
  "tha-ap308:3.4_0": "subījaṁ vāpa ropaya’.",
  "tha-ap308:4.1_0": "Idaṁ vatvāna sabbaññū,",
  "tha-ap308:4.2_0": "lokajeṭṭho narāsabho;",
  "tha-ap308:4.3_0": "Mameva anusāsitvā,",
  "tha-ap308:4.4_0": "vehāsaṁ nabhamuggami.",
  "tha-ap308:5.1_0": "Aciraṁ gatamattamhi,",
  "tha-ap308:5.2_0": "sabbaññumhi narāsabhe;",
  "tha-ap308:5.3_0": "Maraṇaṁ samanuppatto,",
  "tha-ap308:5.4_0": "tusitaṁ upapajjahaṁ.",
  "tha-ap308:6.1_0": "Tadāhaṁ viraje khette,",
  "tha-ap308:6.2_0": "anantaphaladāyake;",
  "tha-ap308:6.3_0": "Saṅghe cittaṁ pasādetvā,",
  "tha-ap308:6.4_0": "kappaṁ saggamhi modahaṁ.",
  "tha-ap308:7.1_0": "Catunnavutito kappe,",
  "tha-ap308:7.2_0": "pasādamalabhiṁ tadā;",
  "tha-ap308:7.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap308:7.4_0": "pasādassa idaṁ phalaṁ.",
  "tha-ap308:8.1_0": "Paṭisambhidā catasso,",
  "tha-ap308:8.2_0": "…pe…",
  "tha-ap308:8.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap308:9.1_0": "Itthaṁ sudaṁ āyasmā sampasādako thero imā gāthāyo abhāsitthāti.",
  "tha-ap308:10.1_0": "Sampasādakattherassāpadānaṁ chaṭṭhaṁ."
}