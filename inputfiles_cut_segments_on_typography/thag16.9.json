{
  "thag16.9:0.1_0": "Theragāthā 16.9",
  "thag16.9:0.2_0": "Vīsatinipāta",
  "thag16.9:0.3_0": "Paṭhamavagga",
  "thag16.9:0.4_0": "Anuruddhattheragāthā",
  "thag16.9:1.1_0": "“Pahāya mātāpitaro,",
  "thag16.9:1.2_0": "bhaginī ñātibhātaro;",
  "thag16.9:1.3_0": "Pañca kāmaguṇe hitvā,",
  "thag16.9:1.4_0": "anuruddhova jhāyati.",
  "thag16.9:2.1_0": "Sameto naccagītehi,",
  "thag16.9:2.2_0": "sammatāḷappabodhano;",
  "thag16.9:2.3_0": "Na tena suddhimajjhagaṁ,",
  "thag16.9:2.4_0": "mārassa visaye rato.",
  "thag16.9:3.1_0": "Etañca samatikkamma,",
  "thag16.9:3.2_0": "rato buddhassa sāsane;",
  "thag16.9:3.3_0": "Sabboghaṁ samatikkamma,",
  "thag16.9:3.4_0": "anuruddhova jhāyati.",
  "thag16.9:4.1_0": "Rūpā saddā rasā gandhā,",
  "thag16.9:4.2_0": "phoṭṭhabbā ca manoramā;",
  "thag16.9:4.3_0": "Ete ca samatikkamma,",
  "thag16.9:4.4_0": "anuruddhova jhāyati.",
  "thag16.9:5.1_0": "Piṇḍapātamatikkanto,",
  "thag16.9:5.2_0": "eko adutiyo muni;",
  "thag16.9:5.3_0": "Esati paṁsukūlāni,",
  "thag16.9:5.4_0": "anuruddho anāsavo.",
  "thag16.9:6.1_0": "Vicinī aggahī dhovi,",
  "thag16.9:6.2_0": "rajayī dhārayī muni;",
  "thag16.9:6.3_0": "Paṁsukūlāni matimā,",
  "thag16.9:6.4_0": "anuruddho anāsavo.",
  "thag16.9:7.1_0": "Mahiccho ca asantuṭṭho,",
  "thag16.9:7.2_0": "saṁsaṭṭho yo ca uddhato;",
  "thag16.9:7.3_0": "Tassa dhammā ime honti,",
  "thag16.9:7.4_0": "pāpakā saṅkilesikā.",
  "thag16.9:8.1_0": "Sato ca hoti appiccho,",
  "thag16.9:8.2_0": "santuṭṭho avighātavā;",
  "thag16.9:8.3_0": "Pavivekarato vitto,",
  "thag16.9:8.4_0": "niccamāraddhavīriyo.",
  "thag16.9:9.1_0": "Tassa dhammā ime honti,",
  "thag16.9:9.2_0": "kusalā bodhipakkhikā;",
  "thag16.9:9.3_0": "Anāsavo ca so hoti,",
  "thag16.9:9.4_0": "iti vuttaṁ mahesinā.",
  "thag16.9:10.1_0": "Mama saṅkappamaññāya,",
  "thag16.9:10.2_0": "satthā loke anuttaro;",
  "thag16.9:10.3_0": "Manomayena kāyena,",
  "thag16.9:10.4_0": "iddhiyā upasaṅkami.",
  "thag16.9:11.1_0": "Yadā me ahu saṅkappo,",
  "thag16.9:11.2_0": "tato uttari desayi;",
  "thag16.9:11.3_0": "Nippapañcarato buddho,",
  "thag16.9:11.4_0": "nippapañcamadesayi.",
  "thag16.9:12.1_0": "Tassāhaṁ dhammamaññāya,",
  "thag16.9:12.2_0": "vihāsiṁ sāsane rato;",
  "thag16.9:12.3_0": "Tisso vijjā anuppattā,",
  "thag16.9:12.4_0": "kataṁ buddhassa sāsanaṁ.",
  "thag16.9:13.1_0": "Pañcapaññāsavassāni,",
  "thag16.9:13.2_0": "yato nesajjiko ahaṁ;",
  "thag16.9:13.3_0": "Pañcavīsativassāni,",
  "thag16.9:13.4_0": "yato middhaṁ samūhataṁ.",
  "thag16.9:14.1_0": "Nāhu assāsapassāsā,",
  "thag16.9:14.2_0": "ṭhitacittassa tādino;",
  "thag16.9:14.3_0": "Anejo santimārabbha,",
  "thag16.9:14.4_0": "cakkhumā parinibbuto.",
  "thag16.9:15.1_0": "Asallīnena cittena,",
  "thag16.9:15.2_0": "vedanaṁ ajjhavāsayi;",
  "thag16.9:15.3_0": "Pajjotasseva nibbānaṁ,",
  "thag16.9:15.4_0": "vimokkho cetaso ahu.",
  "thag16.9:16.1_0": "Ete pacchimakā dāni,",
  "thag16.9:16.2_0": "munino phassapañcamā;",
  "thag16.9:16.3_0": "Nāññe dhammā bhavissanti,",
  "thag16.9:16.4_0": "sambuddhe parinibbute.",
  "thag16.9:17.1_0": "Natthi dāni punāvāso,",
  "thag16.9:17.2_0": "devakāyasmi jālini;",
  "thag16.9:17.3_0": "Vikkhīṇo jātisaṁsāro,",
  "thag16.9:17.4_0": "natthi dāni punabbhavo.",
  "thag16.9:18.1_0": "Yassa muhuttena sahassadhā,",
  "thag16.9:18.2_0": "Loko saṁvidito sabrahmakappo;",
  "thag16.9:18.3_0": "Vasī iddhiguṇe cutūpapāte,",
  "thag16.9:18.4_0": "Kāle passati devatā sa bhikkhu.",
  "thag16.9:19.1_0": "Annabhāro pure āsiṁ,",
  "thag16.9:19.2_0": "daliddo ghāsahārako;",
  "thag16.9:19.3_0": "Samaṇaṁ paṭipādesiṁ,",
  "thag16.9:19.4_0": "upariṭṭhaṁ yasassinaṁ.",
  "thag16.9:20.1_0": "Somhi sakyakule jāto,",
  "thag16.9:20.2_0": "anuruddhoti maṁ vidū;",
  "thag16.9:20.3_0": "Upeto naccagītehi,",
  "thag16.9:20.4_0": "sammatāḷappabodhano.",
  "thag16.9:21.1_0": "Athaddasāsiṁ sambuddhaṁ,",
  "thag16.9:21.2_0": "satthāraṁ akutobhayaṁ;",
  "thag16.9:21.3_0": "Tasmiṁ cittaṁ pasādetvā,",
  "thag16.9:21.4_0": "pabbajiṁ anagāriyaṁ.",
  "thag16.9:22.1_0": "Pubbenivāsaṁ jānāmi,",
  "thag16.9:22.2_0": "yattha me vusitaṁ pure;",
  "thag16.9:22.3_0": "Tāvatiṁsesu devesu,",
  "thag16.9:22.4_0": "aṭṭhāsiṁ sakkajātiyā.",
  "thag16.9:23.1_0": "Sattakkhattuṁ manussindo,",
  "thag16.9:23.2_0": "ahaṁ rajjamakārayiṁ;",
  "thag16.9:23.3_0": "Cāturanto vijitāvī,",
  "thag16.9:23.4_0": "jambusaṇḍassa issaro;",
  "thag16.9:23.5_0": "Adaṇḍena asatthena,",
  "thag16.9:23.6_0": "dhammena anusāsayiṁ.",
  "thag16.9:24.1_0": "Ito satta tato satta,",
  "thag16.9:24.2_0": "saṁsārāni catuddasa;",
  "thag16.9:24.3_0": "Nivāsamabhijānissaṁ,",
  "thag16.9:24.4_0": "devaloke ṭhito tadā.",
  "thag16.9:25.1_0": "Pañcaṅgike samādhimhi,",
  "thag16.9:25.2_0": "sante ekodibhāvite;",
  "thag16.9:25.3_0": "Paṭippassaddhiladdhamhi,",
  "thag16.9:25.4_0": "dibbacakkhu visujjhi me.",
  "thag16.9:26.1_0": "Cutūpapātaṁ jānāmi,",
  "thag16.9:26.2_0": "sattānaṁ āgatiṁ gatiṁ;",
  "thag16.9:26.3_0": "Itthabhāvaññathābhāvaṁ,",
  "thag16.9:26.4_0": "jhāne pañcaṅgike ṭhito.",
  "thag16.9:27.1_0": "Pariciṇṇo mayā satthā,",
  "thag16.9:27.2_0": "… pe",
  "thag16.9:27.4_0": "bhavanetti samūhatā.",
  "thag16.9:28.1_0": "Vajjīnaṁ veḷuvagāme,",
  "thag16.9:28.2_0": "ahaṁ jīvitasaṅkhayā;",
  "thag16.9:28.3_0": "Heṭṭhato veḷugumbasmiṁ,",
  "thag16.9:28.4_0": "nibbāyissaṁ anāsavo”ti.",
  "thag16.9:28.5_0": "… Anuruddho thero …."
}