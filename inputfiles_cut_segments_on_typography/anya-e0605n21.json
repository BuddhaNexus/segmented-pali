{
  "anya-e0605n21:0_0": "Mahāpadhānadīpanīgāthā",
  "anya-e0605n21:1_0": "111.",
  "anya-e0605n21:2_0": "Orohitotohitapāpadhammo,",
  "anya-e0605n21:3_0": "Channena sa channahayena gantvā;",
  "anya-e0605n21:4_0": "Anomatīramhi anomasatto,",
  "anya-e0605n21:5_0": "Anomapabbajjamupāgato so.",
  "anya-e0605n21:6_0": "112.",
  "anya-e0605n21:7_0": "Nirāmisaṁ pītisukhaṁ anūpamaṁ,",
  "anya-e0605n21:8_0": "Anūpiye ambavane alattha;",
  "anya-e0605n21:9_0": "Sarūpasobhāya virūpasobhaṁ,",
  "anya-e0605n21:10_0": "Sarājikaṁ rājagahaṁ karittha.",
  "anya-e0605n21:11_0": "113.",
  "anya-e0605n21:12_0": "Tato aḷāra ūdakatāpasānaṁ,",
  "anya-e0605n21:13_0": "Jhānenasantuṭṭhamano vihāya;",
  "anya-e0605n21:14_0": "Mahāpadhānāya uruvelabhūmiṁ,",
  "anya-e0605n21:15_0": "Gato sikhappattamakāsi dukkaraṁ.",
  "anya-e0605n21:16_0": "114.",
  "anya-e0605n21:17_0": "Na kāmato nevatidukkaramhi,",
  "anya-e0605n21:18_0": "Sabbaññutā sijjhati majjhimāya;",
  "anya-e0605n21:19_0": "Ñatvāna taṁ pubbaguṇopaladdhaṁ,",
  "anya-e0605n21:20_0": "Dhammaṁ samānetumagā subodhiṁ."
}