{
  "dn20:0.1_0": "Dīgha Nikāya 20",
  "dn20:0.2_0": "Mahāsamayasutta",
  "dn20:1.1_0": "Evaṁ me sutaṁ—",
  "dn20:1.2_0": "ekaṁ samayaṁ bhagavā sakkesu viharati kapilavatthusmiṁ mahāvane mahatā bhikkhusaṅghena saddhiṁ pañcamattehi bhikkhusatehi sabbeheva arahantehi;",
  "dn20:1.3_0": "dasahi ca lokadhātūhi devatā yebhuyyena sannipatitā honti bhagavantaṁ dassanāya bhikkhusaṅghañca.",
  "dn20:2.1_0": "Atha kho catunnaṁ suddhāvāsakāyikānaṁ devatānaṁ etadahosi:",
  "dn20:2.2_0": "“ayaṁ kho bhagavā sakkesu viharati kapilavatthusmiṁ mahāvane mahatā bhikkhusaṅghena saddhiṁ pañcamattehi bhikkhusatehi sabbeheva arahantehi;",
  "dn20:2.3_0": "dasahi ca lokadhātūhi devatā yebhuyyena sannipatitā honti bhagavantaṁ dassanāya bhikkhusaṅghañca.",
  "dn20:2.4_0": "Yannūna mayampi yena bhagavā tenupasaṅkameyyāma;",
  "dn20:2.4_1": "upasaṅkamitvā bhagavato santike paccekaṁ gāthaṁ bhāseyyāmā”ti.",
  "dn20:3.1_0": "Atha kho tā devatā seyyathāpi nāma balavā puriso samiñjitaṁ vā bāhaṁ pasāreyya pasāritaṁ vā bāhaṁ samiñjeyya;",
  "dn20:3.1_1": "evameva—suddhāvāsesu devesu antarahitā bhagavato purato pāturahesuṁ.",
  "dn20:3.2_0": "Atha kho tā devatā bhagavantaṁ abhivādetvā ekamantaṁ aṭṭhaṁsu.",
  "dn20:3.3_0": "Ekamantaṁ ṭhitā kho ekā devatā bhagavato santike imaṁ gāthaṁ abhāsi:",
  "dn20:3.4_0": "“Mahāsamayo pavanasmiṁ,",
  "dn20:3.5_0": "Devakāyā samāgatā;",
  "dn20:3.6_0": "Āgatamha imaṁ dhammasamayaṁ,",
  "dn20:3.7_0": "Dakkhitāye aparājitasaṅghan”ti.",
  "dn20:3.8_0": "Atha kho aparā devatā bhagavato santike imaṁ gāthaṁ abhāsi:",
  "dn20:3.9_0": "“Tatra bhikkhavo samādahaṁsu,",
  "dn20:3.10_0": "Cittamattano ujukaṁ akaṁsu;",
  "dn20:3.11_0": "Sārathīva nettāni gahetvā,",
  "dn20:3.12_0": "Indriyāni rakkhanti paṇḍitā”ti.",
  "dn20:3.13_0": "Atha kho aparā devatā bhagavato santike imaṁ gāthaṁ abhāsi:",
  "dn20:3.14_0": "“Chetvā khīlaṁ chetvā palighaṁ,",
  "dn20:3.15_0": "Indakhīlaṁ ūhacca manejā;",
  "dn20:3.16_0": "Te caranti suddhā vimalā,",
  "dn20:3.17_0": "Cakkhumatā sudantā susunāgā”ti.",
  "dn20:3.18_0": "Atha kho aparā devatā bhagavato santike imaṁ gāthaṁ abhāsi:",
  "dn20:3.19_0": "“Ye keci buddhaṁ saraṇaṁ gatāse,",
  "dn20:3.20_0": "Na te gamissanti apāyabhūmiṁ;",
  "dn20:3.21_0": "Pahāya mānusaṁ dehaṁ,",
  "dn20:3.22_0": "Devakāyaṁ paripūressantī”ti.",
  "dn20:4.0_0": "1. Devatāsannipāta",
  "dn20:4.1_0": "Atha kho bhagavā bhikkhū āmantesi:",
  "dn20:4.2_0": "“yebhuyyena,",
  "dn20:4.2_1": "bhikkhave,",
  "dn20:4.2_2": "dasasu lokadhātūsu devatā sannipatitā honti,",
  "dn20:4.2_3": "tathāgataṁ dassanāya bhikkhusaṅghañca.",
  "dn20:4.3_0": "Yepi te,",
  "dn20:4.3_1": "bhikkhave,",
  "dn20:4.3_2": "ahesuṁ atītamaddhānaṁ arahanto sammāsambuddhā,",
  "dn20:4.3_3": "tesampi bhagavantānaṁ etapparamāyeva devatā sannipatitā ahesuṁ seyyathāpi mayhaṁ etarahi.",
  "dn20:4.4_0": "Yepi te,",
  "dn20:4.4_1": "bhikkhave,",
  "dn20:4.4_2": "bhavissanti anāgatamaddhānaṁ arahanto sammāsambuddhā,",
  "dn20:4.4_3": "tesampi bhagavantānaṁ etapparamāyeva devatā sannipatitā bhavissanti seyyathāpi mayhaṁ etarahi.",
  "dn20:4.5_0": "Ācikkhissāmi, bhikkhave, devakāyānaṁ nāmāni;",
  "dn20:4.6_0": "kittayissāmi, bhikkhave, devakāyānaṁ nāmāni;",
  "dn20:4.7_0": "desessāmi, bhikkhave, devakāyānaṁ nāmāni.",
  "dn20:4.8_0": "Taṁ suṇātha, sādhukaṁ manasikarotha, bhāsissāmī”ti.",
  "dn20:4.9_0": "“Evaṁ, bhante”ti kho te bhikkhū bhagavato paccassosuṁ.",
  "dn20:4.10_0": "Bhagavā etadavoca:",
  "dn20:5.1_0": "“Silokamanukassāmi,",
  "dn20:5.2_0": "yattha bhummā tadassitā;",
  "dn20:5.3_0": "Ye sitā girigabbharaṁ,",
  "dn20:5.4_0": "pahitattā samāhitā.",
  "dn20:5.5_0": "Puthūsīhāva sallīnā,",
  "dn20:5.6_0": "lomahaṁsābhisambhuno;",
  "dn20:5.7_0": "Odātamanasā suddhā,",
  "dn20:5.8_0": "vippasannamanāvilā.",
  "dn20:5.9_0": "Bhiyyo pañcasate ñatvā,",
  "dn20:5.10_0": "vane kāpilavatthave;",
  "dn20:5.11_0": "Tato āmantayī satthā,",
  "dn20:5.12_0": "sāvake sāsane rate.",
  "dn20:5.13_0": "‘Devakāyā abhikkantā,",
  "dn20:5.14_0": "te vijānātha bhikkhavo’;",
  "dn20:5.15_0": "Te ca ātappamakaruṁ,",
  "dn20:5.16_0": "sutvā buddhassa sāsanaṁ.",
  "dn20:6.1_0": "Tesaṁ pāturahu ñāṇaṁ,",
  "dn20:6.2_0": "amanussānadassanaṁ;",
  "dn20:6.3_0": "Appeke satamaddakkhuṁ,",
  "dn20:6.4_0": "sahassaṁ atha sattariṁ.",
  "dn20:6.5_0": "Sataṁ eke sahassānaṁ,",
  "dn20:6.6_0": "amanussānamaddasuṁ;",
  "dn20:6.7_0": "Appekenantamaddakkhuṁ,",
  "dn20:6.8_0": "disā sabbā phuṭā ahuṁ.",
  "dn20:6.9_0": "Tañca sabbaṁ abhiññāya,",
  "dn20:6.10_0": "vavatthitvāna cakkhumā;",
  "dn20:6.11_0": "Tato āmantayī satthā,",
  "dn20:6.12_0": "sāvake sāsane rate.",
  "dn20:6.13_0": "‘Devakāyā abhikkantā,",
  "dn20:6.14_0": "te vijānātha bhikkhavo;",
  "dn20:6.15_0": "Ye vohaṁ kittayissāmi,",
  "dn20:6.16_0": "girāhi anupubbaso’.",
  "dn20:7.1_0": "Sattasahassā te yakkhā,",
  "dn20:7.2_0": "bhummā kāpilavatthavā;",
  "dn20:7.3_0": "Iddhimanto jutimanto,",
  "dn20:7.4_0": "vaṇṇavanto yasassino;",
  "dn20:7.5_0": "Modamānā abhikkāmuṁ,",
  "dn20:7.6_0": "bhikkhūnaṁ samitiṁ vanaṁ.",
  "dn20:7.7_0": "Chasahassā hemavatā,",
  "dn20:7.8_0": "yakkhā nānattavaṇṇino;",
  "dn20:7.9_0": "Iddhimanto jutimanto,",
  "dn20:7.10_0": "vaṇṇavanto yasassino;",
  "dn20:7.11_0": "Modamānā abhikkāmuṁ,",
  "dn20:7.12_0": "bhikkhūnaṁ samitiṁ vanaṁ.",
  "dn20:7.13_0": "Sātāgirā tisahassā,",
  "dn20:7.14_0": "yakkhā nānattavaṇṇino;",
  "dn20:7.15_0": "Iddhimanto jutimanto,",
  "dn20:7.16_0": "vaṇṇavanto yasassino;",
  "dn20:7.17_0": "Modamānā abhikkāmuṁ,",
  "dn20:7.18_0": "bhikkhūnaṁ samitiṁ vanaṁ.",
  "dn20:7.19_0": "Iccete soḷasasahassā,",
  "dn20:7.20_0": "yakkhā nānattavaṇṇino;",
  "dn20:7.21_0": "Iddhimanto jutimanto,",
  "dn20:7.22_0": "vaṇṇavanto yasassino;",
  "dn20:7.23_0": "Modamānā abhikkāmuṁ,",
  "dn20:7.24_0": "bhikkhūnaṁ samitiṁ vanaṁ.",
  "dn20:8.1_0": "Vessāmittā pañcasatā,",
  "dn20:8.2_0": "yakkhā nānattavaṇṇino;",
  "dn20:8.3_0": "Iddhimanto jutimanto,",
  "dn20:8.4_0": "vaṇṇavanto yasassino;",
  "dn20:8.5_0": "Modamānā abhikkāmuṁ,",
  "dn20:8.6_0": "bhikkhūnaṁ samitiṁ vanaṁ.",
  "dn20:8.7_0": "Kumbhīro rājagahiko,",
  "dn20:8.8_0": "vepullassa nivesanaṁ;",
  "dn20:8.9_0": "Bhiyyo naṁ satasahassaṁ,",
  "dn20:8.10_0": "yakkhānaṁ payirupāsati;",
  "dn20:8.11_0": "Kumbhīro rājagahiko,",
  "dn20:8.12_0": "sopāgā samitiṁ vanaṁ.",
  "dn20:9.1_0": "Purimañca disaṁ rājā,",
  "dn20:9.2_0": "dhataraṭṭho pasāsati;",
  "dn20:9.3_0": "Gandhabbānaṁ adhipati,",
  "dn20:9.4_0": "mahārājā yasassiso.",
  "dn20:9.5_0": "Puttāpi tassa bahavo,",
  "dn20:9.6_0": "indanāmā mahabbalā;",
  "dn20:9.7_0": "Iddhimanto jutimanto,",
  "dn20:9.8_0": "vaṇṇavanto yasassino;",
  "dn20:9.9_0": "Modamānā abhikkāmuṁ,",
  "dn20:9.10_0": "bhikkhūnaṁ samitiṁ vanaṁ.",
  "dn20:9.11_0": "Dakkhiṇañca disaṁ rājā,",
  "dn20:9.12_0": "virūḷho taṁ pasāsati;",
  "dn20:9.13_0": "Kumbhaṇḍānaṁ adhipati,",
  "dn20:9.14_0": "mahārājā yasassiso.",
  "dn20:9.15_0": "Puttāpi tassa bahavo,",
  "dn20:9.16_0": "indanāmā mahabbalā;",
  "dn20:9.17_0": "Iddhimanto jutimanto,",
  "dn20:9.18_0": "vaṇṇavanto yasassino;",
  "dn20:9.19_0": "Modamānā abhikkāmuṁ,",
  "dn20:9.20_0": "bhikkhūnaṁ samitiṁ vanaṁ.",
  "dn20:9.21_0": "Pacchimañca disaṁ rājā,",
  "dn20:9.22_0": "virūpakkho pasāsati;",
  "dn20:9.23_0": "Nāgānañca adhipati,",
  "dn20:9.24_0": "mahārājā yasassiso.",
  "dn20:9.25_0": "Puttāpi tassa bahavo,",
  "dn20:9.26_0": "indanāmā mahabbalā;",
  "dn20:9.27_0": "Iddhimanto jutimanto,",
  "dn20:9.28_0": "vaṇṇavanto yasassino;",
  "dn20:9.29_0": "Modamānā abhikkāmuṁ,",
  "dn20:9.30_0": "bhikkhūnaṁ samitiṁ vanaṁ.",
  "dn20:9.31_0": "Uttarañca disaṁ rājā,",
  "dn20:9.32_0": "kuvero taṁ pasāsati;",
  "dn20:9.33_0": "Yakkhānañca adhipati,",
  "dn20:9.34_0": "mahārājā yasassiso.",
  "dn20:9.35_0": "Puttāpi tassa bahavo,",
  "dn20:9.36_0": "indanāmā mahabbalā;",
  "dn20:9.37_0": "Iddhimanto jutimanto,",
  "dn20:9.38_0": "vaṇṇavanto yasassino;",
  "dn20:9.39_0": "Modamānā abhikkāmuṁ,",
  "dn20:9.40_0": "bhikkhūnaṁ samitiṁ vanaṁ.",
  "dn20:9.41_0": "Purimaṁ disaṁ dhataraṭṭho,",
  "dn20:9.42_0": "dakkhiṇena virūḷhako;",
  "dn20:9.43_0": "Pacchimena virūpakkho,",
  "dn20:9.44_0": "kuvero uttaraṁ disaṁ.",
  "dn20:9.45_0": "Cattāro te mahārājā,",
  "dn20:9.46_0": "samantā caturo disā;",
  "dn20:9.47_0": "Daddallamānā aṭṭhaṁsu,",
  "dn20:9.48_0": "vane kāpilavatthave.",
  "dn20:10.1_0": "Tesaṁ māyāvino dāsā,",
  "dn20:10.2_0": "āguṁ vañcanikā saṭhā;",
  "dn20:10.3_0": "Māyā kuṭeṇḍu viṭeṇḍu,",
  "dn20:10.4_0": "viṭucca viṭuṭo saha.",
  "dn20:10.5_0": "Candano kāmaseṭṭho ca,",
  "dn20:10.6_0": "kinnighaṇḍu nighaṇḍu ca;",
  "dn20:10.7_0": "Panādo opamañño ca,",
  "dn20:10.8_0": "devasūto ca mātali.",
  "dn20:10.9_0": "Cittaseno ca gandhabbo,",
  "dn20:10.10_0": "naḷorājā janesabho;",
  "dn20:10.11_0": "Āgā pañcasikho ceva,",
  "dn20:10.12_0": "timbarū sūriyavacchasā.",
  "dn20:10.13_0": "Ete caññe ca rājāno,",
  "dn20:10.14_0": "gandhabbā saha rājubhi;",
  "dn20:10.15_0": "Modamānā abhikkāmuṁ,",
  "dn20:10.16_0": "bhikkhūnaṁ samitiṁ vanaṁ.",
  "dn20:11.1_0": "Athāguṁ nāgasā nāgā,",
  "dn20:11.2_0": "vesālā sahatacchakā;",
  "dn20:11.3_0": "Kambalassatarā āguṁ,",
  "dn20:11.4_0": "pāyāgā saha ñātibhi.",
  "dn20:11.5_0": "Yāmunā dhataraṭṭhā ca,",
  "dn20:11.6_0": "āgū nāgā yasassino;",
  "dn20:11.7_0": "Erāvaṇo mahānāgo,",
  "dn20:11.8_0": "sopāgā samitiṁ vanaṁ.",
  "dn20:11.9_0": "Ye nāgarāje sahasā haranti,",
  "dn20:11.10_0": "Dibbā dijā pakkhi visuddhacakkhū;",
  "dn20:11.11_0": "Vehāyasā te vanamajjhapattā,",
  "dn20:11.12_0": "Citrā supaṇṇā iti tesa nāmaṁ.",
  "dn20:11.13_0": "Abhayaṁ tadā nāgarājānamāsi,",
  "dn20:11.14_0": "Supaṇṇato khemamakāsi buddho;",
  "dn20:11.15_0": "Saṇhāhi vācāhi upavhayantā,",
  "dn20:11.16_0": "Nāgā supaṇṇā saraṇamakaṁsu buddhaṁ.",
  "dn20:12.1_0": "Jitā vajirahatthena,",
  "dn20:12.2_0": "samuddaṁ asurāsitā;",
  "dn20:12.3_0": "Bhātaro vāsavassete,",
  "dn20:12.4_0": "iddhimanto yasassino.",
  "dn20:12.5_0": "Kālakañcā mahābhismā,",
  "dn20:12.6_0": "asurā dānaveghasā;",
  "dn20:12.7_0": "Vepacitti sucitti ca,",
  "dn20:12.8_0": "pahārādo namucī saha.",
  "dn20:12.9_0": "Satañca baliputtānaṁ,",
  "dn20:12.10_0": "sabbe verocanāmakā;",
  "dn20:12.11_0": "Sannayhitvā balisenaṁ,",
  "dn20:12.12_0": "rāhubhaddamupāgamuṁ;",
  "dn20:12.13_0": "‘Samayo dāni bhaddante,",
  "dn20:12.14_0": "bhikkhūnaṁ samitiṁ vanaṁ’.",
  "dn20:13.1_0": "Āpo ca devā pathavī,",
  "dn20:13.2_0": "tejo vāyo tadāgamuṁ;",
  "dn20:13.3_0": "Varuṇā vāraṇā devā,",
  "dn20:13.4_0": "somo ca yasasā saha.",
  "dn20:13.5_0": "Mettā karuṇā kāyikā,",
  "dn20:13.6_0": "āguṁ devā yasassino;",
  "dn20:13.7_0": "Dasete dasadhā kāyā,",
  "dn20:13.8_0": "sabbe nānattavaṇṇino.",
  "dn20:13.9_0": "Iddhimanto jutimanto,",
  "dn20:13.10_0": "vaṇṇavanto yasassino;",
  "dn20:13.11_0": "Modamānā abhikkāmuṁ,",
  "dn20:13.12_0": "bhikkhūnaṁ samitiṁ vanaṁ.",
  "dn20:14.1_0": "Veṇḍudevā sahali ca,",
  "dn20:14.2_0": "asamā ca duve yamā;",
  "dn20:14.3_0": "Candassūpanisā devā,",
  "dn20:14.4_0": "candamāguṁ purakkhatvā.",
  "dn20:14.5_0": "Sūriyassūpanisā devā,",
  "dn20:14.6_0": "sūriyamāguṁ purakkhatvā;",
  "dn20:14.7_0": "Nakkhattāni purakkhatvā,",
  "dn20:14.8_0": "āguṁ mandavalāhakā.",
  "dn20:14.9_0": "Vasūnaṁ vāsavo seṭṭho,",
  "dn20:14.10_0": "sakkopāgā purindado;",
  "dn20:14.11_0": "Dasete dasadhā kāyā,",
  "dn20:14.12_0": "sabbe nānattavaṇṇino.",
  "dn20:14.13_0": "Iddhimanto jutimanto,",
  "dn20:14.14_0": "vaṇṇavanto yasassino;",
  "dn20:14.15_0": "Modamānā abhikkāmuṁ,",
  "dn20:14.16_0": "bhikkhūnaṁ samitiṁ vanaṁ.",
  "dn20:15.1_0": "Athāguṁ sahabhū devā,",
  "dn20:15.2_0": "jalamaggi sikhāriva;",
  "dn20:15.3_0": "Ariṭṭhakā ca rojā ca,",
  "dn20:15.4_0": "umāpupphanibhāsino.",
  "dn20:15.5_0": "Varuṇā sahadhammā ca,",
  "dn20:15.6_0": "accutā ca anejakā;",
  "dn20:15.7_0": "Sūleyyarucirā āguṁ,",
  "dn20:15.8_0": "āguṁ vāsavanesino;",
  "dn20:15.9_0": "Dasete dasadhā kāyā,",
  "dn20:15.10_0": "sabbe nānattavaṇṇino.",
  "dn20:15.11_0": "Iddhimanto jutimanto,",
  "dn20:15.12_0": "vaṇṇavanto yasassino;",
  "dn20:15.13_0": "Modamānā abhikkāmuṁ,",
  "dn20:15.14_0": "bhikkhūnaṁ samitiṁ vanaṁ.",
  "dn20:16.1_0": "Samānā mahāsamanā,",
  "dn20:16.2_0": "mānusā mānusuttamā;",
  "dn20:16.3_0": "Khiḍḍāpadosikā āguṁ,",
  "dn20:16.4_0": "āguṁ manopadosikā.",
  "dn20:16.5_0": "Athāguṁ harayo devā,",
  "dn20:16.6_0": "ye ca lohitavāsino;",
  "dn20:16.7_0": "Pāragā mahāpāragā,",
  "dn20:16.8_0": "āguṁ devā yasassino;",
  "dn20:16.9_0": "Dasete dasadhā kāyā,",
  "dn20:16.10_0": "sabbe nānattavaṇṇino.",
  "dn20:16.11_0": "Iddhimanto jutimanto,",
  "dn20:16.12_0": "vaṇṇavanto yasassino;",
  "dn20:16.13_0": "Modamānā abhikkāmuṁ,",
  "dn20:16.14_0": "bhikkhūnaṁ samitiṁ vanaṁ.",
  "dn20:17.1_0": "Sukkā karambhā aruṇā,",
  "dn20:17.2_0": "āguṁ veghanasā saha;",
  "dn20:17.3_0": "Odātagayhā pāmokkhā,",
  "dn20:17.4_0": "āguṁ devā vicakkhaṇā.",
  "dn20:17.5_0": "Sadāmattā hāragajā,",
  "dn20:17.6_0": "missakā ca yasassino;",
  "dn20:17.7_0": "Thanayaṁ āga pajjunno,",
  "dn20:17.8_0": "yo disā abhivassati.",
  "dn20:17.9_0": "Dasete dasadhā kāyā,",
  "dn20:17.10_0": "sabbe nānattavaṇṇino;",
  "dn20:17.11_0": "Iddhimanto jutimanto,",
  "dn20:17.12_0": "vaṇṇavanto yasassino;",
  "dn20:17.13_0": "Modamānā abhikkāmuṁ,",
  "dn20:17.14_0": "bhikkhūnaṁ samitiṁ vanaṁ.",
  "dn20:18.1_0": "Khemiyā tusitā yāmā,",
  "dn20:18.2_0": "kaṭṭhakā ca yasassino;",
  "dn20:18.3_0": "Lambītakā lāmaseṭṭhā,",
  "dn20:18.4_0": "jotināmā ca āsavā;",
  "dn20:18.5_0": "Nimmānaratino āguṁ,",
  "dn20:18.6_0": "athāguṁ paranimmitā.",
  "dn20:18.7_0": "Dasete dasadhā kāyā,",
  "dn20:18.8_0": "sabbe nānattavaṇṇino;",
  "dn20:18.9_0": "Iddhimanto jutimanto,",
  "dn20:18.10_0": "vaṇṇavanto yasassino;",
  "dn20:18.11_0": "Modamānā abhikkāmuṁ,",
  "dn20:18.12_0": "bhikkhūnaṁ samitiṁ vanaṁ.",
  "dn20:19.1_0": "Saṭṭhete devanikāyā,",
  "dn20:19.2_0": "sabbe nānattavaṇṇino;",
  "dn20:19.3_0": "Nāmanvayena āgacchuṁ,",
  "dn20:19.4_0": "ye caññe sadisā saha.",
  "dn20:19.5_0": "‘Pavuṭṭhajātimakhilaṁ,",
  "dn20:19.6_0": "oghatiṇṇamanāsavaṁ;",
  "dn20:19.7_0": "Dakkhemoghataraṁ nāgaṁ,",
  "dn20:19.8_0": "candaṁva asitātigaṁ’.",
  "dn20:20.1_0": "Subrahmā paramatto ca,",
  "dn20:20.2_0": "puttā iddhimato saha;",
  "dn20:20.3_0": "Sanaṅkumāro tisso ca,",
  "dn20:20.4_0": "sopāga samitiṁ vanaṁ.",
  "dn20:20.5_0": "Sahassaṁ brahmalokānaṁ,",
  "dn20:20.6_0": "mahābrahmābhitiṭṭhati;",
  "dn20:20.7_0": "Upapanno jutimanto,",
  "dn20:20.8_0": "bhismākāyo yasassiso.",
  "dn20:20.9_0": "Dasettha issarā āguṁ,",
  "dn20:20.10_0": "paccekavasavattino;",
  "dn20:20.11_0": "Tesañca majjhato āga,",
  "dn20:20.12_0": "hārito parivārito.",
  "dn20:21.1_0": "Te ca sabbe abhikkante,",
  "dn20:21.2_0": "sainde deve sabrahmake;",
  "dn20:21.3_0": "Mārasenā abhikkāmi,",
  "dn20:21.4_0": "passa kaṇhassa mandiyaṁ.",
  "dn20:21.5_0": "‘Etha gaṇhatha bandhatha,",
  "dn20:21.6_0": "rāgena baddhamatthu vo;",
  "dn20:21.7_0": "Samantā parivāretha,",
  "dn20:21.8_0": "mā vo muñcittha koci naṁ’.",
  "dn20:21.9_0": "Iti tattha mahāseno,",
  "dn20:21.10_0": "kaṇho senaṁ apesayi;",
  "dn20:21.11_0": "Pāṇinā talamāhacca,",
  "dn20:21.12_0": "saraṁ katvāna bheravaṁ.",
  "dn20:21.13_0": "Yathā pāvussako megho,",
  "dn20:21.14_0": "thanayanto savijjuko;",
  "dn20:21.15_0": "Tadā so paccudāvatti,",
  "dn20:21.16_0": "saṅkuddho asayaṁvase.",
  "dn20:22.1_0": "Tañca sabbaṁ abhiññāya,",
  "dn20:22.2_0": "vavatthitvāna cakkhumā;",
  "dn20:22.3_0": "Tato āmantayī satthā,",
  "dn20:22.4_0": "sāvake sāsane rate.",
  "dn20:22.5_0": "‘Mārasenā abhikkantā,",
  "dn20:22.6_0": "te vijānātha bhikkhavo’;",
  "dn20:22.7_0": "Te ca ātappamakaruṁ,",
  "dn20:22.8_0": "sutvā buddhassa sāsanaṁ;",
  "dn20:22.9_0": "Vītarāgehi pakkāmuṁ,",
  "dn20:22.10_0": "nesaṁ lomāpi iñjayuṁ.",
  "dn20:22.11_0": "‘Sabbe vijitasaṅgāmā,",
  "dn20:22.12_0": "bhayātītā yasassino;",
  "dn20:22.13_0": "Modanti saha bhūtehi,",
  "dn20:22.14_0": "sāvakā te janesutā’”ti.",
  "dn20:22.15_0": "Mahāsamayasuttaṁ niṭṭhitaṁ sattamaṁ."
}