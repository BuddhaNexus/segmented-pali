{
  "ja372:0.1_0": "Jātaka",
  "ja372:0.2_0": "Pañcakanipāta",
  "ja372:0.3_0": "Aḍḍhavagga",
  "ja372:0.4_0": "2. Migapotakajātaka",
  "ja372:1.1_0": "“Agārā paccupetassa,",
  "ja372:1.2_0": "anagārassa te sato;",
  "ja372:1.3_0": "Samaṇassa na taṁ sādhu,",
  "ja372:1.4_0": "yaṁ petamanusocasi”.",
  "ja372:2.1_0": "“Saṁvāsena have sakka,",
  "ja372:2.2_0": "manussassa migassa vā;",
  "ja372:2.3_0": "Hadaye jāyate pemaṁ,",
  "ja372:2.4_0": "na taṁ sakkā asocituṁ”.",
  "ja372:3.1_0": "“Mataṁ marissaṁ rodanti,",
  "ja372:3.2_0": "ye rudanti lapanti ca;",
  "ja372:3.3_0": "Tasmā tvaṁ isi mā rodi,",
  "ja372:3.4_0": "roditaṁ moghamāhu santo.",
  "ja372:4.1_0": "Roditena have brahme,",
  "ja372:4.2_0": "mato peto samuṭṭhahe;",
  "ja372:4.3_0": "Sabbe saṅgamma rodāma,",
  "ja372:4.4_0": "aññamaññassa ñātake”.",
  "ja372:5.1_0": "“Ādittaṁ vata maṁ santaṁ,",
  "ja372:5.2_0": "ghatasittaṁva pāvakaṁ;",
  "ja372:5.3_0": "Vārinā viya osiñcaṁ,",
  "ja372:5.4_0": "sabbaṁ nibbāpaye daraṁ.",
  "ja372:6.1_0": "Abbahi vata me sallaṁ,",
  "ja372:6.2_0": "yamāsi hadayassitaṁ;",
  "ja372:6.3_0": "Yo me sokaparetassa,",
  "ja372:6.4_0": "puttasokaṁ apānudi.",
  "ja372:7.1_0": "Sohaṁ abbūḷhasallosmi,",
  "ja372:7.2_0": "vītasoko anāvilo;",
  "ja372:7.3_0": "Na socāmi na rodāmi,",
  "ja372:7.4_0": "tava sutvāna vāsavā”ti.",
  "ja372:8.1_0": "Migapotakajātakaṁ dutiyaṁ."
}