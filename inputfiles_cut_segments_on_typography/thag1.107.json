{
  "thag1.107:0.1_0": "Theragāthā 1.107",
  "thag1.107:0.2_0": "Ekakanipāta",
  "thag1.107:0.3_0": "Ekādasamavagga",
  "thag1.107:0.4_0": "Dhammasavattheragāthā",
  "thag1.107:1.1_0": "“Pabbajiṁ tulayitvāna,",
  "thag1.107:1.2_0": "agārasmānagāriyaṁ;",
  "thag1.107:1.3_0": "Tisso vijjā anuppattā,",
  "thag1.107:1.4_0": "kataṁ buddhassa sāsanan”ti.",
  "thag1.107:1.5_0": "… Dhammasavo thero …."
}