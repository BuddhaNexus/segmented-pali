{
  "thag1.79:0.1_0": "Theragāthā 1.79",
  "thag1.79:0.2_0": "Ekakanipāta",
  "thag1.79:0.3_0": "Aṭṭhamavagga",
  "thag1.79:0.4_0": "Rakkhitattheragāthā",
  "thag1.79:1.1_0": "“Sabbo rāgo pahīno me,",
  "thag1.79:1.2_0": "sabbo doso samūhato;",
  "thag1.79:1.3_0": "Sabbo me vigato moho,",
  "thag1.79:1.4_0": "sītibhūtosmi nibbuto”ti.",
  "thag1.79:1.5_0": "… Rakkhito thero …."
}