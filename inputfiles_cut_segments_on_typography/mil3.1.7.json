{
  "mil3.1.7:0.1_0": "Milindapañha",
  "mil3.1.7:0.2_0": "Mahāvagga",
  "mil3.1.7:0.3_0": "7. Yonisomanasikārapañha",
  "mil3.1.7:1.1_0": "Rājā āha—",
  "mil3.1.7:1.2_0": "“bhante nāgasena, yo na paṭisandahati, nanu so yoniso manasikārena na paṭisandahatī”ti?",
  "mil3.1.7:1.3_0": "“Yoniso ca, mahārāja, manasikārena paññāya ca aññehi ca kusalehi dhammehī”ti.",
  "mil3.1.7:1.4_0": "“Nanu, bhante, yoniso manasikāroyeva paññā”ti?",
  "mil3.1.7:1.5_0": "“Na hi, mahārāja,",
  "mil3.1.7:1.5_1": "añño manasikāro,",
  "mil3.1.7:1.5_2": "aññā paññā,",
  "mil3.1.7:1.5_3": "imesaṁ kho, mahārāja,",
  "mil3.1.7:1.5_4": "ajeḷakagoṇamahiṁsaoṭṭhagadrabhānampi manasikāro atthi,",
  "mil3.1.7:1.5_5": "paññā pana tesaṁ natthī”ti.",
  "mil3.1.7:2.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.1.7:3.1_0": "Yonisomanasikārapañho sattamo."
}