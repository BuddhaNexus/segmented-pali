{
  "thag1.13:0.1_0": "Theragāthā 1.13",
  "thag1.13:0.2_0": "Ekakanipāta",
  "thag1.13:0.3_0": "Dutiyavagga",
  "thag1.13:0.4_0": "Vanavacchattheragāthā",
  "thag1.13:1.1_0": "“Nīlabbhavaṇṇā rucirā,",
  "thag1.13:1.2_0": "sītavārī sucindharā;",
  "thag1.13:1.3_0": "Indagopakasañchannā,",
  "thag1.13:1.4_0": "te selā ramayanti man”ti.",
  "thag1.13:1.5_0": "… Vanavaccho thero …."
}