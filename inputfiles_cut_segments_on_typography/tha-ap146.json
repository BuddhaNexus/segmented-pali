{
  "tha-ap146:0.1_0": "Therāpadāna",
  "tha-ap146:0.2_0": "Chattavagga",
  "tha-ap146:0.3_0": "4. Saparivāriyattheraapadāna",
  "tha-ap146:1.1_0": "“Padumuttaro nāma jino,",
  "tha-ap146:1.2_0": "lokajeṭṭho narāsabho;",
  "tha-ap146:1.3_0": "Jalitvā aggikkhandhova,",
  "tha-ap146:1.4_0": "sambuddho parinibbuto.",
  "tha-ap146:2.1_0": "Nibbute ca mahāvīre,",
  "tha-ap146:2.2_0": "thūpo vitthāriko ahu;",
  "tha-ap146:2.3_0": "Dūratova upaṭṭhenti,",
  "tha-ap146:2.4_0": "dhātugehavaruttame.",
  "tha-ap146:3.1_0": "Pasannacitto sumano,",
  "tha-ap146:3.2_0": "akaṁ candanavedikaṁ;",
  "tha-ap146:3.3_0": "Dissati thūpakhandho ca,",
  "tha-ap146:3.4_0": "thūpānucchaviko tadā.",
  "tha-ap146:4.1_0": "Bhave nibbattamānamhi,",
  "tha-ap146:4.2_0": "devatte atha mānuse;",
  "tha-ap146:4.3_0": "Omattaṁ me na passāmi,",
  "tha-ap146:4.4_0": "pubbakammassidaṁ phalaṁ.",
  "tha-ap146:5.1_0": "Pañcadasakappasate,",
  "tha-ap146:5.2_0": "ito aṭṭha janā ahuṁ;",
  "tha-ap146:5.3_0": "Sabbe samattanāmā te,",
  "tha-ap146:5.4_0": "cakkavattī mahabbalā.",
  "tha-ap146:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap146:6.2_0": "…pe…",
  "tha-ap146:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap146:7.1_0": "Itthaṁ sudaṁ āyasmā saparivāriyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap146:8.1_0": "Saparivāriyattherassāpadānaṁ catutthaṁ."
}