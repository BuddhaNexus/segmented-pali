{
  "tha-ap356:0.1_0": "Therāpadāna",
  "tha-ap356:0.2_0": "Saddasaññakavagga",
  "tha-ap356:0.3_0": "4 Sakosakakoraṇḍadāyakattheraapadāna",
  "tha-ap356:1.1_0": "“Akkantañca padaṁ disvā,",
  "tha-ap356:1.2_0": "sikhino lokabandhuno;",
  "tha-ap356:1.3_0": "Ekaṁsaṁ ajinaṁ katvā,",
  "tha-ap356:1.4_0": "padaseṭṭhaṁ avandahaṁ.",
  "tha-ap356:2.1_0": "Koraṇḍaṁ pupphitaṁ disvā,",
  "tha-ap356:2.2_0": "pādapaṁ dharaṇīruhaṁ;",
  "tha-ap356:2.3_0": "Sakosakaṁ gahetvāna,",
  "tha-ap356:2.4_0": "padacakkaṁ apūjayiṁ.",
  "tha-ap356:3.1_0": "Ekattiṁse ito kappe,",
  "tha-ap356:3.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap356:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap356:3.4_0": "padapūjāyidaṁ phalaṁ.",
  "tha-ap356:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap356:4.2_0": "…pe…",
  "tha-ap356:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap356:5.1_0": "Itthaṁ sudaṁ āyasmā sakosakakoraṇḍadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap356:6.1_0": "Sakosakakoraṇḍadāyakattherassāpadānaṁ catutthaṁ."
}