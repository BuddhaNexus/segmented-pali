{
  "atk-s0403a39:0_0": "1. Dhanavaggo",
  "atk-s0403a39:1_0": "1-5. Paṭhamapiyasuttādivaṇṇanā",
  "atk-s0403a39:2_0": "1- 5.",
  "atk-s0403a39:2_1": "Sattakanipātassa paṭhame anavaññattikāmoti abhiññātabhāvakāmo.",
  "atk-s0403a39:2_2": "Tatiye yoniso vicine dhammanti upāyena catusaccadhammaṁ vicināti.",
  "atk-s0403a39:2_3": "Paññāyatthaṁ vipassatīti sahavipassanāya maggapaññāya saccadhammaṁ vipassati.",
  "atk-s0403a39:2_4": "Pajjotassevāti dīpasseva.",
  "atk-s0403a39:2_5": "Vimokkho hoti cetasoti tassa imehi balehi samannāgatassa khīṇāsavassa dīpanibbānaṁ viya carimakacittassa vatthārammaṇehi vimokkho hoti,",
  "atk-s0403a39:2_6": "gataṭṭhānaṁ na paññāyati.",
  "atk-s0403a39:2_7": "Catutthe saddho hotītiādīni pañcakanipāte vaṇṇitāneva.",
  "atk-s0403a39:2_8": "Pañcame dhanānīti adāliddiyakaraṇaṭṭhena dhanāni.",
  "atk-s0403a39:3_0": "7. Uggasuttavaṇṇanā",
  "atk-s0403a39:4_0": "7.",
  "atk-s0403a39:4_1": "Sattame uggo rājamahāmattoti pasenadikosalassa mahāamacco.",
  "atk-s0403a39:4_2": "Upasaṅkamīti bhuttapātarāso upasaṅkami.",
  "atk-s0403a39:4_3": "Aḍḍhoti nidhānagatena dhanena aḍḍho.",
  "atk-s0403a39:4_4": "Migārorohaṇeyyoti rohaṇaseṭṭhino nattāraṁ migāraseṭṭhiṁ sandhāyevamāha.",
  "atk-s0403a39:4_5": "Mahaddhanoti vaḷañjanadhanena mahaddhano.",
  "atk-s0403a39:4_6": "Mahābhogoti upabhogaparibhogabhaṇḍassa mahantatāya mahābhogo.",
  "atk-s0403a39:4_7": "Hiraññassāti suvaṇṇasseva.",
  "atk-s0403a39:4_8": "Suvaṇṇāmeva hissa koṭisaṅkhyaṁ ahosi.",
  "atk-s0403a39:4_9": "Rūpiyassāti sesassa taṭṭakasarakaattharaṇapāvuraṇādino paribhogaparikkhārassa pamāṇasaṅkhāne vādoyeva natthi.",
  "atk-s0403a39:5_0": "8. Saṁyojanasuttavaṇṇanā",
  "atk-s0403a39:6_0": "8.",
  "atk-s0403a39:6_1": "Aṭṭhame anunayasaṁyojananti kāmarāgasaṁyojanaṁ.",
  "atk-s0403a39:6_2": "Sabbāneva cetāni bandhanaṭṭhena saṁyojanānīti veditabbāni.",
  "atk-s0403a39:6_3": "Imasmiṁ sutte vaṭṭameva kathitaṁ.",
  "atk-s0403a39:6_4": "Sesaṁ sabbattha uttānatthamevāti.",
  "atk-s0403a39:7_0": "Dhanavaggo paṭhamo."
}