{
  "thag2.41:0.1_0": "Theragāthā 2.41",
  "thag2.41:0.2_0": "Dukanipāta",
  "thag2.41:0.3_0": "Pañcamavagga",
  "thag2.41:0.4_0": "Kumārakassapattheragāthā",
  "thag2.41:1.1_0": "“Aho buddhā aho dhammā,",
  "thag2.41:1.2_0": "aho no satthu sampadā;",
  "thag2.41:1.3_0": "Yattha etādisaṁ dhammaṁ,",
  "thag2.41:1.4_0": "sāvako sacchikāhiti.",
  "thag2.41:2.1_0": "Asaṅkheyyesu kappesu,",
  "thag2.41:2.2_0": "sakkāyādhigatā ahū;",
  "thag2.41:2.3_0": "Tesamayaṁ pacchimako,",
  "thag2.41:2.4_0": "carimoyaṁ samussayo;",
  "thag2.41:2.5_0": "Jātimaraṇasaṁsāro,",
  "thag2.41:2.6_0": "natthi dāni punabbhavo”ti.",
  "thag2.41:2.7_0": "… Kumārakassapo thero …."
}