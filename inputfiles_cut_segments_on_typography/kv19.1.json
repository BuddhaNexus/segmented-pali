{
  "kv19.1:0.1_0": "Kathāvatthu",
  "kv19.1:0.2_0": "Niggahapaṇṇāsaka",
  "kv19.1:0.3_0": "Ekūnavīsatimavagga",
  "kv19.1:0.4_0": "Kilesajahanakathā",
  "kv19.1:1.1_0": "Atīte kilese jahatīti?",
  "kv19.1:1.2_0": "Āmantā.",
  "kv19.1:1.3_0": "Niruddhaṁ nirodheti,",
  "kv19.1:1.3_1": "vigataṁ vigameti,",
  "kv19.1:1.3_2": "khīṇaṁ khepeti,",
  "kv19.1:1.3_3": "atthaṅgataṁ atthaṅgameti,",
  "kv19.1:1.3_4": "abbhatthaṅgataṁ abbhatthaṅgametīti?",
  "kv19.1:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv19.1:1.5_0": "atīte kilese jahatīti?",
  "kv19.1:1.6_0": "Āmantā.",
  "kv19.1:1.7_0": "Nanu atītaṁ niruddhanti?",
  "kv19.1:1.8_0": "Āmantā.",
  "kv19.1:1.9_0": "Hañci atītaṁ niruddhaṁ, no ca vata re vattabbe—",
  "kv19.1:1.10_0": "“atīte kilese jahatī”ti.",
  "kv19.1:1.11_0": "Atīte kilese jahatīti?",
  "kv19.1:1.12_0": "Āmantā.",
  "kv19.1:1.13_0": "Nanu atītaṁ natthīti?",
  "kv19.1:1.14_0": "Āmantā.",
  "kv19.1:1.15_0": "Hañci atītaṁ natthi, no ca vata re vattabbe—",
  "kv19.1:1.16_0": "“atīte kilese jahatī”ti.",
  "kv19.1:2.1_0": "Anāgate kilese jahatīti?",
  "kv19.1:2.2_0": "Āmantā.",
  "kv19.1:2.3_0": "Ajātaṁ ajaneti, asañjātaṁ asañjaneti, anibbattaṁ anibbatteti, apātubhūtaṁ apātubhāvetīti?",
  "kv19.1:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv19.1:2.5_0": "anāgate kilese jahatīti?",
  "kv19.1:2.6_0": "Āmantā.",
  "kv19.1:2.7_0": "Nanu anāgataṁ ajātanti?",
  "kv19.1:2.8_0": "Āmantā.",
  "kv19.1:2.9_0": "Hañci anāgataṁ ajātaṁ, no ca vata re vattabbe—",
  "kv19.1:2.10_0": "“anāgate kilese jahatī”ti.",
  "kv19.1:2.11_0": "Anāgate kilese jahatīti?",
  "kv19.1:2.12_0": "Āmantā.",
  "kv19.1:2.13_0": "Nanu anāgataṁ natthīti?",
  "kv19.1:2.14_0": "Āmantā.",
  "kv19.1:2.15_0": "Hañci anāgataṁ natthi, no ca vata re vattabbe—",
  "kv19.1:2.16_0": "“anāgate kilese jahatī”ti.",
  "kv19.1:3.1_0": "Paccuppanne kilese jahatīti?",
  "kv19.1:3.2_0": "Āmantā.",
  "kv19.1:3.3_0": "Ratto rāgaṁ jahati, duṭṭho dosaṁ jahati, mūḷho mohaṁ jahati, kiliṭṭho kilese jahatīti?",
  "kv19.1:3.4_0": "Na hevaṁ vattabbe …pe…",
  "kv19.1:3.5_0": "rāgena rāgaṁ jahati, dosena dosaṁ jahati, mohena mohaṁ jahati, kilesehi kilese jahatīti?",
  "kv19.1:3.6_0": "Na hevaṁ vattabbe …pe…",
  "kv19.1:4.1_0": "Rāgo cittasampayutto, maggo cittasampayuttoti?",
  "kv19.1:4.2_0": "Āmantā.",
  "kv19.1:4.3_0": "Dvinnaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv19.1:4.4_0": "Na hevaṁ vattabbe …pe….",
  "kv19.1:5.1_0": "Rāgo akusalo, maggo kusaloti?",
  "kv19.1:5.2_0": "Āmantā.",
  "kv19.1:5.3_0": "Kusalākusalā sāvajjānavajjā hīnapaṇītā kaṇhasukkasappaṭibhāgā dhammā sammukhībhāvaṁ āgacchantīti?",
  "kv19.1:5.4_0": "Na hevaṁ vattabbe …pe….",
  "kv19.1:6.1_0": "Kusalākusalā sāvajjānavajjā hīnapaṇītā kaṇhasukkasappaṭibhāgā dhammā sammukhībhāvaṁ āgacchantīti?",
  "kv19.1:6.2_0": "Āmantā.",
  "kv19.1:6.3_0": "Nanu vuttaṁ bhagavatā—",
  "kv19.1:6.4_0": "“cattārimāni, bhikkhave, suvidūravidūrāni.",
  "kv19.1:6.5_0": "Katamāni cattāri?",
  "kv19.1:6.6_0": "Nabhañca, bhikkhave, pathavī ca—",
  "kv19.1:6.7_0": "idaṁ paṭhamaṁ suvidūravidūraṁ …pe… tasmā sataṁ dhammo asabbhi ārakā”ti.",
  "kv19.1:6.8_0": "Attheva suttantoti?",
  "kv19.1:6.9_0": "Āmantā.",
  "kv19.1:6.10_0": "Tena hi na vattabbaṁ—",
  "kv19.1:6.11_0": "“kusalākusalā …pe…",
  "kv19.1:6.12_0": "sammukhībhāvaṁ āgacchantī”ti.",
  "kv19.1:7.1_0": "Na vattabbaṁ—",
  "kv19.1:7.2_0": "“atīte kilese jahati, anāgate kilese jahati, paccuppanne kilese jahatī”ti?",
  "kv19.1:7.3_0": "Āmantā.",
  "kv19.1:7.4_0": "Natthi kilese jahatīti?",
  "kv19.1:7.5_0": "Na hevaṁ vattabbe …pe…",
  "kv19.1:7.6_0": "tena hi atīte kilese jahati, anāgate kilese jahati, paccuppanne kilese jahatīti.",
  "kv19.1:7.7_0": "Kilesajahanakathā niṭṭhitā."
}