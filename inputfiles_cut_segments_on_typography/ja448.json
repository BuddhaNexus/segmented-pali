{
  "ja448:0.1_0": "Jātaka",
  "ja448:0.2_0": "Dasakanipāta",
  "ja448:0.3_0": "Catudvāravagga",
  "ja448:0.4_0": "10. Kukkuṭajātaka",
  "ja448:1.1_0": "“Nāsmase katapāpamhi,",
  "ja448:1.2_0": "nāsmase alikavādine;",
  "ja448:1.3_0": "Nāsmase attatthapaññamhi,",
  "ja448:1.4_0": "atisantepi nāsmase.",
  "ja448:2.1_0": "Bhavanti heke purisā,",
  "ja448:2.2_0": "gopipāsikajātikā;",
  "ja448:2.3_0": "Ghasanti maññe mittāni,",
  "ja448:2.4_0": "vācāya na ca kammunā.",
  "ja448:3.1_0": "Sukkhañjalipaggahitā,",
  "ja448:3.2_0": "vācāya paliguṇṭhitā;",
  "ja448:3.3_0": "Manussapheggū nāsīde,",
  "ja448:3.4_0": "yasmiṁ natthi kataññutā.",
  "ja448:4.1_0": "Na hi aññaññacittānaṁ,",
  "ja448:4.2_0": "itthīnaṁ purisāna vā;",
  "ja448:4.3_0": "Nānāvikatvā saṁsaggaṁ,",
  "ja448:4.4_0": "tādisampi ca nāsmase.",
  "ja448:5.1_0": "Anariyakammamokkantaṁ,",
  "ja448:5.2_0": "athetaṁ sabbaghātinaṁ;",
  "ja448:5.3_0": "Nisitaṁva paṭicchannaṁ,",
  "ja448:5.4_0": "tādisampi ca nāsmase.",
  "ja448:6.1_0": "Mittarūpenidhekacce,",
  "ja448:6.2_0": "sākhalyena acetasā;",
  "ja448:6.3_0": "Vividhehi upāyanti,",
  "ja448:6.4_0": "tādisampi ca nāsmase.",
  "ja448:7.1_0": "Āmisaṁ vā dhanaṁ vāpi,",
  "ja448:7.2_0": "yattha passati tādiso;",
  "ja448:7.3_0": "Dubbhiṁ karoti dummedho,",
  "ja448:7.4_0": "tañca hantvāna gacchati”.",
  "ja448:8.1_0": "“Mittarūpena bahavo,",
  "ja448:8.2_0": "channā sevanti sattavo;",
  "ja448:8.3_0": "Jahe kāpurise hete,",
  "ja448:8.4_0": "kukkuṭo viya senakaṁ.",
  "ja448:9.1_0": "Yo ca uppatitaṁ atthaṁ,",
  "ja448:9.2_0": "na khippamanubujjhati;",
  "ja448:9.3_0": "Amittavasamanveti,",
  "ja448:9.4_0": "pacchā ca anutappati.",
  "ja448:10.1_0": "Yo ca uppatitaṁ atthaṁ,",
  "ja448:10.2_0": "khippameva nibodhati;",
  "ja448:10.3_0": "Muccate sattusambādhā,",
  "ja448:10.4_0": "kukkuṭo viya senakā.",
  "ja448:11.1_0": "Taṁ tādisaṁ kūṭamivoḍḍitaṁ vane,",
  "ja448:11.2_0": "Adhammikaṁ niccavidhaṁsakārinaṁ;",
  "ja448:11.3_0": "Ārā vivajjeyya naro vicakkhaṇo,",
  "ja448:11.4_0": "Senaṁ yathā kukkuṭo vaṁsakānane”ti.",
  "ja448:12.1_0": "Kukkuṭajātakaṁ dasamaṁ."
}