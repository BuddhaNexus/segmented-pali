{
  "pli-tv-bu-vb-sk10:0.1_0": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk10:0.2_0": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk10:0.3_0": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk10:0.4_0": "Parimaṇḍalavagga",
  "pli-tv-bu-vb-sk10:0.5_0": "10. Dutiyaukkhittakasikkhāpada",
  "pli-tv-bu-vb-sk10:1.1_0": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk10:1.2_0": "Tena kho pana samayena chabbaggiyā bhikkhū ukkhittakāya antaraghare nisīdanti …pe… .",
  "pli-tv-bu-vb-sk10:1.3.1_0": "“Na ukkhittakāya antaraghare nisīdissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk10:1.4_0": "Na ukkhittakāya antaraghare nisīditabbaṁ.",
  "pli-tv-bu-vb-sk10:1.5_0": "Yo anādariyaṁ paṭicca ekato vā ubhato vā ukkhipitvā antaraghare nisīdati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk10:1.6.1_0": "Anāpatti—",
  "pli-tv-bu-vb-sk10:1.7_0": "asañcicca,",
  "pli-tv-bu-vb-sk10:1.8_0": "assatiyā,",
  "pli-tv-bu-vb-sk10:1.9_0": "ajānantassa,",
  "pli-tv-bu-vb-sk10:1.10_0": "gilānassa,",
  "pli-tv-bu-vb-sk10:1.11_0": "vāsūpagatassa,",
  "pli-tv-bu-vb-sk10:1.12_0": "āpadāsu,",
  "pli-tv-bu-vb-sk10:1.13_0": "ummattakassa,",
  "pli-tv-bu-vb-sk10:1.14_0": "ādikammikassāti.",
  "pli-tv-bu-vb-sk10:1.15_0": "Dasamasikkhāpadaṁ niṭṭhitaṁ.",
  "pli-tv-bu-vb-sk10:1.16_0": "Parimaṇḍalavaggo paṭhamo."
}