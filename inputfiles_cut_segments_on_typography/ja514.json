{
  "ja514:0.1_0": "Jātaka",
  "ja514:0.2_0": "Tiṁsanipāta",
  "ja514:0.3_0": "Kiṁchandavagga",
  "ja514:0.4_0": "4. Chaddantajātaka",
  "ja514:1.1_0": "“Kiṁ nu socasinuccaṅgi,",
  "ja514:1.2_0": "paṇḍūsi varavaṇṇini;",
  "ja514:1.3_0": "Milāyasi visālakkhi,",
  "ja514:1.4_0": "mālāva parimadditā”.",
  "ja514:2.1_0": "“Dohaḷo me mahārāja,",
  "ja514:2.2_0": "supinantenupajjhagā;",
  "ja514:2.3_0": "Na so sulabharūpova,",
  "ja514:2.4_0": "yādiso mama dohaḷo”.",
  "ja514:3.1_0": "“Ye keci mānusā kāmā,",
  "ja514:3.2_0": "idha lokasmi nandane;",
  "ja514:3.3_0": "Sabbe te pacurā mayhaṁ,",
  "ja514:3.4_0": "ahaṁ te dammi dohaḷaṁ”.",
  "ja514:4.1_0": "“Luddā deva samāyantu,",
  "ja514:4.2_0": "ye keci vijite tava;",
  "ja514:4.3_0": "Etesaṁ ahamakkhissaṁ,",
  "ja514:4.4_0": "yādiso mama dohaḷo”.",
  "ja514:5.1_0": "“Ime te luddakā devi,",
  "ja514:5.2_0": "katahatthā visāradā;",
  "ja514:5.3_0": "Vanaññū ca migaññū ca,",
  "ja514:5.4_0": "mamatthe cattajīvitā”.",
  "ja514:6.1_0": "“Luddaputtā nisāmetha,",
  "ja514:6.2_0": "yāvantettha samāgatā;",
  "ja514:6.3_0": "Chabbisāṇaṁ gajaṁ setaṁ,",
  "ja514:6.4_0": "addasaṁ supine ahaṁ;",
  "ja514:6.5_0": "Tassa dantehi me attho,",
  "ja514:6.6_0": "alābhe natthi jīvitaṁ”.",
  "ja514:7.1_0": "“Na no pitūnaṁ na pitāmahānaṁ,",
  "ja514:7.2_0": "Diṭṭho suto kuñjaro chabbisāṇo;",
  "ja514:7.3_0": "Yamaddasā supine rājaputtī,",
  "ja514:7.4_0": "Akkhāhi no yādiso hatthināgo”.",
  "ja514:8.1_0": "“Disā catasso vidisā catasso,",
  "ja514:8.2_0": "Uddhaṁ adho dasa disā imāyo;",
  "ja514:8.3_0": "Katamaṁ disaṁ tiṭṭhati nāgarājā,",
  "ja514:8.4_0": "Yamaddasā supine chabbisāṇaṁ”.",
  "ja514:9.1_0": "“Ito ujuṁ uttariyaṁ disāyaṁ,",
  "ja514:9.2_0": "Atikkamma so sattagirī brahante;",
  "ja514:9.3_0": "Suvaṇṇapasso nāma girī uḷāro,",
  "ja514:9.4_0": "Supupphito kimpurisānuciṇṇo.",
  "ja514:10.1_0": "Āruyha selaṁ bhavanaṁ kinnarānaṁ,",
  "ja514:10.2_0": "Olokaya pabbatapādamūlaṁ;",
  "ja514:10.3_0": "Atha dakkhasī meghasamānavaṇṇaṁ,",
  "ja514:10.4_0": "Nigrodharājaṁ aṭṭhasahassapādaṁ.",
  "ja514:11.1_0": "Tatthacchatī kuñjaro chabbisāṇo,",
  "ja514:11.2_0": "Sabbaseto duppasaho parebhi;",
  "ja514:11.3_0": "Rakkhanti naṁ aṭṭhasahassanāgā,",
  "ja514:11.4_0": "Īsādantā vātajavappahārino.",
  "ja514:12.1_0": "Tiṭṭhanti te tumūlaṁ passasantā,",
  "ja514:12.2_0": "Kuppanti vātassapi eritassa;",
  "ja514:12.3_0": "Manussabhūtaṁ pana tattha disvā,",
  "ja514:12.4_0": "Bhasmaṁ kareyyuṁ nāssa rajopi tassa”.",
  "ja514:13.1_0": "“Bahū hime rājakulamhi santi,",
  "ja514:13.2_0": "Piḷandhanā jātarūpassa devī;",
  "ja514:13.3_0": "Muttā maṇī veḷuriyāmayā ca,",
  "ja514:13.4_0": "Kiṁ kāhasi dantapiḷandhanena;",
  "ja514:13.5_0": "Māretukāmā kuñjaraṁ chabbisāṇaṁ,",
  "ja514:13.6_0": "Udāhu ghātessasi luddaputte”.",
  "ja514:14.1_0": "“Sā issitā dukkhitā casmi ludda,",
  "ja514:14.2_0": "Uddhañca sussāmi anussarantī;",
  "ja514:14.3_0": "Karohi me luddaka etamatthaṁ,",
  "ja514:14.4_0": "Dassāmi te gāmavarāni pañca”.",
  "ja514:15.1_0": "“Katthacchatī kattha mupeti ṭhānaṁ,",
  "ja514:15.2_0": "Vīthissa kā nhāna gatassa hoti;",
  "ja514:15.3_0": "Kathañhi so nhāyati nāgarājā,",
  "ja514:15.4_0": "Kathaṁ vijānemu gatiṁ gajassa”.",
  "ja514:16.1_0": "“Tattheva sā pokkharaṇī adūre,",
  "ja514:16.2_0": "Rammā sutitthā ca mahodikā ca;",
  "ja514:16.3_0": "Sampupphitā bhamaragaṇānuciṇṇā,",
  "ja514:16.4_0": "Ettha hi so nhāyati nāgarājā.",
  "ja514:17.1_0": "Sīsaṁ nahātuppalamālabhārī,",
  "ja514:17.2_0": "Sabbaseto puṇḍarīkattacaṅgī;",
  "ja514:17.3_0": "Āmodamāno gacchati sanniketaṁ,",
  "ja514:17.4_0": "Purakkhatvā mahesiṁ sabbabhaddaṁ”.",
  "ja514:18.1_0": "Tattheva so uggahetvāna vākyaṁ,",
  "ja514:18.2_0": "Ādāya tūṇiñca dhanuñca luddo;",
  "ja514:18.3_0": "Vituriyati sattagirī brahante,",
  "ja514:18.4_0": "Suvaṇṇapassaṁ nāma giriṁ uḷāraṁ.",
  "ja514:19.1_0": "Āruyha selaṁ bhavanaṁ kinnarānaṁ,",
  "ja514:19.2_0": "Olokayī pabbatapādamūlaṁ;",
  "ja514:19.3_0": "Tatthaddasā meghasamānavaṇṇaṁ,",
  "ja514:19.4_0": "Nigrodharājaṁ aṭṭhasahassapādaṁ.",
  "ja514:20.1_0": "Tatthaddasā kuñjaraṁ chabbisāṇaṁ,",
  "ja514:20.2_0": "Sabbasetaṁ duppasahaṁ parebhi;",
  "ja514:20.3_0": "Rakkhanti naṁ aṭṭhasahassanāgā,",
  "ja514:20.4_0": "Īsādantā vātajavappahārino.",
  "ja514:21.1_0": "Tatthaddasā pokkharaṇiṁ adūre,",
  "ja514:21.2_0": "Rammaṁ sutitthañca mahodikañca;",
  "ja514:21.3_0": "Sampupphitaṁ bhamaragaṇānuciṇṇaṁ,",
  "ja514:21.4_0": "Yattha hi so nhāyati nāgarājā.",
  "ja514:22.1_0": "Disvāna nāgassa gatiṁ ṭhitiñca,",
  "ja514:22.2_0": "Vīthissa yā nhānagatassa hoti;",
  "ja514:22.3_0": "Opātamāgacchi anariyarūpo,",
  "ja514:22.4_0": "Payojito cittavasānugāya.",
  "ja514:23.1_0": "Khaṇitvāna kāsuṁ phalakehi chādayi,",
  "ja514:23.2_0": "Attānamodhāya dhanuñca luddo;",
  "ja514:23.3_0": "Passāgataṁ puthusallena nāgaṁ,",
  "ja514:23.4_0": "Samappayī dukkaṭakammakārī.",
  "ja514:24.1_0": "Viddho ca nāgo koñcamanādi ghoraṁ,",
  "ja514:24.2_0": "Sabbe ca nāgā ninnaduṁ ghorarūpaṁ;",
  "ja514:24.3_0": "Tiṇañca kaṭṭhañca raṇaṁ karontā,",
  "ja514:24.4_0": "Dhāviṁsu te aṭṭha disā samantato.",
  "ja514:25.1_0": "Vadhissametanti parāmasanto,",
  "ja514:25.2_0": "Kāsāvamaddakkhi dhajaṁ isīnaṁ;",
  "ja514:25.3_0": "Dukkhena phuṭṭhassudapādi saññā,",
  "ja514:25.4_0": "Arahaddhajo sabbhi avajjharūpo.",
  "ja514:26.1_0": "“Anikkasāvo kāsāvaṁ,",
  "ja514:26.2_0": "yo vatthaṁ paridahissati;",
  "ja514:26.3_0": "Apeto damasaccena,",
  "ja514:26.4_0": "na so kāsāvamarahati.",
  "ja514:27.1_0": "Yo ca vantakasāvassa,",
  "ja514:27.2_0": "sīlesu susamāhito;",
  "ja514:27.3_0": "Upeto damasaccena,",
  "ja514:27.4_0": "sa ve kāsāvamarahati”.",
  "ja514:28.1_0": "Samappito puthusallena nāgo,",
  "ja514:28.2_0": "Aduṭṭhacitto luddakamajjhabhāsi;",
  "ja514:28.3_0": "“Kimatthayaṁ kissa vā samma hetu,",
  "ja514:28.4_0": "Mamaṁ vadhī kassa vāyaṁ payogo”.",
  "ja514:29.1_0": "“Kāsissa rañño mahesī bhadante,",
  "ja514:29.2_0": "Sā pūjitā rājakule subhaddā;",
  "ja514:29.3_0": "Taṁ addasā sā ca mamaṁ asaṁsi,",
  "ja514:29.4_0": "Dantehi atthoti ca maṁ avoca”.",
  "ja514:30.1_0": "“Bahū hi me dantayugā uḷārā,",
  "ja514:30.2_0": "Ye me pitūnañca pitāmahānaṁ;",
  "ja514:30.3_0": "Jānāti sā kodhanā rājaputtī,",
  "ja514:30.4_0": "Vadhatthikā veramakāsi bālā.",
  "ja514:31.1_0": "Uṭṭhehi tvaṁ ludda kharaṁ gahetvā,",
  "ja514:31.2_0": "Dante ime chinda purā marāmi;",
  "ja514:31.3_0": "Vajjāsi taṁ kodhanaṁ rājaputtiṁ,",
  "ja514:31.4_0": "‘Nāgo hato handa imassa dantā’”.",
  "ja514:32.1_0": "Uṭṭhāya so luddo kharaṁ gahetvā,",
  "ja514:32.2_0": "Chetvāna dantāni gajuttamassa;",
  "ja514:32.3_0": "Vaggū subhe appaṭime pathabyā,",
  "ja514:32.4_0": "Ādāya pakkāmi tato hi khippaṁ.",
  "ja514:33.1_0": "Bhayaṭṭitā nāgavadhena aṭṭā,",
  "ja514:33.2_0": "Ye te nāgā aṭṭha disā vidhāvuṁ;",
  "ja514:33.3_0": "Adisvāna posaṁ gajapaccamittaṁ,",
  "ja514:33.4_0": "Paccāgamuṁ yena so nāgarājā.",
  "ja514:34.1_0": "Te tattha kanditvā roditvāna nāgā,",
  "ja514:34.2_0": "Sīse sake paṁsukaṁ okiritvā;",
  "ja514:34.3_0": "Agamaṁsu te sabbe sakaṁ niketaṁ,",
  "ja514:34.4_0": "Purakkhatvā mahesiṁ sabbabhaddaṁ.",
  "ja514:35.1_0": "Ādāya dantāni gajuttamassa,",
  "ja514:35.2_0": "Vaggū subhe appaṭime pathabyā;",
  "ja514:35.3_0": "Suvaṇṇarājīhi samantamodare,",
  "ja514:35.4_0": "So luddako kāsipuraṁ upāgami;",
  "ja514:35.5_0": "Upanesi so rājakaññāya dante,",
  "ja514:35.6_0": "Nāgo hato handa imassa dantā.",
  "ja514:36.1_0": "Disvāna dantāni gajuttamassa,",
  "ja514:36.2_0": "Bhattuppiyassa purimāya jātiyā;",
  "ja514:36.3_0": "Tattheva tassā hadayaṁ aphāli,",
  "ja514:36.4_0": "Teneva sā kālamakāsi bālā.",
  "ja514:37.1_0": "Sambodhipatto sa mahānubhāvo,",
  "ja514:37.2_0": "Sitaṁ akāsi parisāya majjhe;",
  "ja514:37.3_0": "Pucchiṁsu bhikkhū suvimuttacittā,",
  "ja514:37.4_0": "“Nākāraṇe pātukaronti buddhā.",
  "ja514:38.1_0": "Yamaddasātha dahariṁ kumāriṁ,",
  "ja514:38.2_0": "Kāsāyavatthaṁ anagāriyaṁ carantiṁ;",
  "ja514:38.3_0": "Sā kho tadā rājakaññā ahosi,",
  "ja514:38.4_0": "Ahaṁ tadā nāgarājā ahosiṁ.",
  "ja514:39.1_0": "Ādāya dantāni gajuttamassa,",
  "ja514:39.2_0": "Vaggū subhe appaṭime pathabyā;",
  "ja514:39.3_0": "Yo luddako kāsipuraṁ upāgami,",
  "ja514:39.4_0": "So kho tadā devadatto ahosi”.",
  "ja514:40.1_0": "Anāvasūraṁ cirarattasaṁsitaṁ,",
  "ja514:40.2_0": "Uccāvacaṁ caritamidaṁ purāṇaṁ;",
  "ja514:40.3_0": "Vītaddaro vītasoko visallo,",
  "ja514:40.4_0": "Sayaṁ abhiññāya abhāsi buddho.",
  "ja514:41.1_0": "“Ahaṁ vo tena kālena,",
  "ja514:41.2_0": "ahosiṁ tattha bhikkhavo;",
  "ja514:41.3_0": "Nāgarājā tadā homi,",
  "ja514:41.4_0": "evaṁ dhāretha jātakan”ti.",
  "ja514:42.1_0": "Chaddantajātakaṁ catutthaṁ."
}