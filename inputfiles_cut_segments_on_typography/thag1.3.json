{
  "thag1.3:0.1_0": "Theragāthā 1.3",
  "thag1.3:0.2_0": "Ekakanipāta",
  "thag1.3:0.3_0": "Paṭhamavagga",
  "thag1.3:0.4_0": "Kaṅkhārevatattheragāthā",
  "thag1.3:1.1_0": "“Paññaṁ imaṁ passa tathāgatānaṁ,",
  "thag1.3:1.2_0": "Aggi yathā pajjalito nisīthe;",
  "thag1.3:1.3_0": "Ālokadā cakkhudadā bhavanti,",
  "thag1.3:1.4_0": "Ye āgatānaṁ vinayanti kaṅkhan”ti.",
  "thag1.3:1.5_0": "Itthaṁ sudaṁ āyasmā kaṅkhārevato thero gāthaṁ abhāsitthāti."
}