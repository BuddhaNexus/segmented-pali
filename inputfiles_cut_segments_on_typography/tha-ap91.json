{
  "tha-ap91:0.1_0": "Therāpadāna",
  "tha-ap91:0.2_0": "Timiravagga",
  "tha-ap91:0.3_0": "9. Bodhisiñcakattheraapadāna",
  "tha-ap91:1.1_0": "“Vipassissa bhagavato,",
  "tha-ap91:1.2_0": "mahābodhimaho ahu;",
  "tha-ap91:1.3_0": "Pabbajjupagato santo,",
  "tha-ap91:1.4_0": "upagacchiṁ ahaṁ tadā.",
  "tha-ap91:2.1_0": "Kusumodakamādāya,",
  "tha-ap91:2.2_0": "bodhiyā okiriṁ ahaṁ;",
  "tha-ap91:2.3_0": "Mocayissati no mutto,",
  "tha-ap91:2.4_0": "nibbāpessati nibbuto.",
  "tha-ap91:3.1_0": "Ekanavutito kappe,",
  "tha-ap91:3.2_0": "yaṁ bodhimabhisiñcayiṁ;",
  "tha-ap91:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap91:3.4_0": "bodhisiñcāyidaṁ phalaṁ.",
  "tha-ap91:4.1_0": "Tettiṁse vattamānamhi,",
  "tha-ap91:4.2_0": "kappe āsuṁ janādhipā;",
  "tha-ap91:4.3_0": "Udakasecanā nāma,",
  "tha-ap91:4.4_0": "aṭṭhete cakkavattino.",
  "tha-ap91:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap91:5.2_0": "…pe…",
  "tha-ap91:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap91:6.1_0": "Itthaṁ sudaṁ āyasmā bodhisiñcako thero imā gāthāyo abhāsitthāti.",
  "tha-ap91:7.1_0": "Bodhisiñcakattherassāpadānaṁ navamaṁ."
}