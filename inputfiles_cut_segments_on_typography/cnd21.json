{
  "cnd21:0.1_0": "Cūḷaniddesa",
  "cnd21:0.2_0": "Pārāyanavagganiddesa",
  "cnd21:0.3_0": "17. Pārāyanatthutigāthāniddesa",
  "cnd21:1.1_0": "<b>Idamavoca bhagavā magadhesu viharanto pāsāṇake cetiye,",
  "cnd21:1.1_1": "paricārakasoḷasānaṁ brāhmaṇānaṁ ajjhiṭṭho puṭṭho puṭṭho pañhaṁ byākāsi.</b>",
  "cnd21:2.1_0": "<b>Idamavoca bhagavā</b>ti idaṁ pārāyanaṁ avoca.",
  "cnd21:2.2_0": "<b>Bhagavā</b>ti gāravādhivacanametaṁ …pe… sacchikā paññatti, yadidaṁ bhagavāti—",
  "cnd21:2.3_0": "idamavoca bhagavā.",
  "cnd21:2.4_0": "<b>Magadhesu viharanto</b>ti magadhanāmake janapade viharanto iriyanto vattento pālento yapento yāpento.",
  "cnd21:2.5_0": "<b>Pāsāṇake cetiye</b>ti pāsāṇakacetiyaṁ vuccati buddhāsananti—",
  "cnd21:2.6_0": "magadhesu viharanto pāsāṇake cetiye.",
  "cnd21:2.7_0": "<b>Paricārakasoḷasānaṁ brāhmaṇānan</b>ti piṅgiyo brāhmaṇo bāvarissa brāhmaṇassa paddho paddhacaro paricārako sisso.",
  "cnd21:2.8_0": "Piṅgiyena te soḷasāti—",
  "cnd21:2.9_0": "evampi paricārakasoḷasānaṁ brāhmaṇānaṁ.",
  "cnd21:2.10_0": "Atha vā te soḷasa brāhmaṇā buddhassa bhagavato paddhā paddhacarā paricārakā sissāti—",
  "cnd21:2.11_0": "evampi paricārakasoḷasānaṁ brāhmaṇānaṁ.",
  "cnd21:3.1_0": "<b>Ajjhiṭṭho puṭṭho puṭṭho pañhaṁ byākāsī</b>ti.",
  "cnd21:3.2_0": "<b>Ajjhiṭṭho</b>ti ajjhiṭṭho ajjhesito.",
  "cnd21:3.3_0": "<b>Puṭṭho puṭṭho</b>ti puṭṭho puṭṭho pucchito pucchito yācito yācito ajjhesito ajjhesito pasādito pasādito.",
  "cnd21:3.4_0": "<b>Pañhaṁ byākāsī</b>ti pañhaṁ byākāsi ācikkhi desesi paññapesi paṭṭhapesi vivari vibhaji uttānīākāsi pakāsesīti—",
  "cnd21:3.5_0": "ajjhiṭṭho puṭṭho puṭṭho pañhaṁ byākāsi.",
  "cnd21:3.6_0": "Tenetaṁ vuccati—",
  "cnd21:4.1_0": "“Idamavoca bhagavā magadhesu viharanto pāsāṇake cetiye,",
  "cnd21:4.1_1": "paricārakasoḷasānaṁ brāhmaṇānaṁ ajjhiṭṭho puṭṭho puṭṭho pañhaṁ byākāsī”ti.",
  "cnd21:5.1_0": "<b>Ekamekassa cepi pañhassa atthamaññāya dhammamaññāya dhammānudhammaṁ paṭipajjeyya,",
  "cnd21:5.1_1": "gaccheyyeva jarāmaraṇassa pāraṁ.</b>",
  "cnd21:5.2_0": "<b>Pāraṅgamanīyā ime dhammāti.</b>",
  "cnd21:5.3_0": "<b>Tasmā imassa dhammapariyāyassa “pārāyanan”teva adhivacanaṁ.</b>",
  "cnd21:6.1_0": "<b>Ekamekassa cepi pañhassā</b>ti ekamekassa cepi ajitapañhassa,",
  "cnd21:6.1_1": "ekamekassa cepi tissametteyyapañhassa,",
  "cnd21:6.1_2": "ekamekassa cepi puṇṇakapañhassa,",
  "cnd21:6.1_3": "ekamekassa cepi mettagūpañhassa,",
  "cnd21:6.1_4": "ekamekassa cepi dhotakapañhassa,",
  "cnd21:6.1_5": "ekamekassa cepi upasīvapañhassa,",
  "cnd21:6.1_6": "ekamekassa cepi nandakapañhassa,",
  "cnd21:6.1_7": "ekamekassa cepi hemakapañhassa,",
  "cnd21:6.1_8": "ekamekassa cepi todeyyapañhassa,",
  "cnd21:6.1_9": "ekamekassa cepi kappapañhassa,",
  "cnd21:6.1_10": "ekamekassa cepi jatukaṇṇipañhassa,",
  "cnd21:6.1_11": "ekamekassa cepi bhadrāvudhapañhassa,",
  "cnd21:6.1_12": "ekamekassa cepi udayapañhassa,",
  "cnd21:6.1_13": "ekamekassa cepi posālapañhassa,",
  "cnd21:6.1_14": "ekamekassa cepi mogharājapañhassa,",
  "cnd21:6.1_15": "ekamekassa cepi piṅgiyapañhassāti—",
  "cnd21:6.2_0": "ekamekassa cepi pañhassa.",
  "cnd21:7.1_0": "<b>Atthamaññāya dhammamaññāyā</b>ti sveva pañho dhammo,",
  "cnd21:7.1_1": "visajjanaṁ atthoti atthaṁ aññāya jānitvā tulayitvā tīrayitvā vibhāvayitvā vibhūtaṁ katvāti—",
  "cnd21:7.2_0": "atthamaññāya.",
  "cnd21:7.3_0": "<b>Dhammamaññāyā</b>ti dhammaṁ aññāya jānitvā tulayitvā tīrayitvā vibhāvayitvā vibhūtaṁ katvāti—",
  "cnd21:7.4_0": "dhammamaññāyāti—",
  "cnd21:7.5_0": "atthamaññāya dhammamaññāya.",
  "cnd21:7.6_0": "<b>Dhammānudhammaṁ paṭipajjeyyā</b>ti sammāpaṭipadaṁ anulomapaṭipadaṁ apaccanīkapaṭipadaṁ anvatthapaṭipadaṁ dhammānudhammapaṭipadaṁ paṭipajjeyyāti—",
  "cnd21:7.7_0": "dhammānudhammaṁ paṭipajjeyya.",
  "cnd21:7.8_0": "<b>Gaccheyyeva jarāmaraṇassa pāran</b>ti jarāmaraṇassa pāraṁ vuccati amataṁ nibbānaṁ.",
  "cnd21:7.9_0": "Yo so sabbasaṅkhārasamatho sabbūpadhipaṭinissaggo taṇhakkhayo virāgo nirodho nibbānaṁ.",
  "cnd21:7.10_0": "<b>Gaccheyyeva jarāmaraṇassa pāran</b>ti jarāmaraṇassa pāraṁ gaccheyya,",
  "cnd21:7.10_1": "pāraṁ adhigaccheyya,",
  "cnd21:7.10_2": "pāraṁ adhiphasseyya,",
  "cnd21:7.10_3": "pāraṁ sacchikareyyāti—",
  "cnd21:7.11_0": "gaccheyyeva jarāmaraṇassa pāraṁ.",
  "cnd21:7.12_0": "<b>Pāraṅgamanīyā ime dhammā</b>ti ime dhammā pāraṅgamanīyā.",
  "cnd21:7.13_0": "Pāraṁ pāpenti pāraṁ sampāpenti pāraṁ samanupāpenti, jarāmaraṇassa taraṇāya saṁvattantīti—",
  "cnd21:7.14_0": "pāraṅgamanīyā ime dhammāti.",
  "cnd21:8.1_0": "<b>Tasmā imassa dhammapariyāyassā</b>ti.",
  "cnd21:8.2_0": "<b>Tasmā</b>ti tasmā taṅkāraṇā taṁhetu tappaccayā taṁnidānāti—",
  "cnd21:8.3_0": "tasmā.",
  "cnd21:8.4_0": "<b>Imassa dhammapariyāyassā</b>ti imassa pārāyanassāti—",
  "cnd21:8.5_0": "tasmā imassa dhammapariyāyassa.",
  "cnd21:8.6_0": "<b>Pārāyananteva adhivacanan</b>ti pāraṁ vuccati amataṁ nibbānaṁ …pe… nirodho nibbānaṁ.",
  "cnd21:8.7_0": "Ayanaṁ vuccati maggo, seyyathidaṁ—",
  "cnd21:8.8_0": "sammādiṭṭhi …pe… sammāsamādhi.",
  "cnd21:8.9_0": "<b>Adhivacanan</b>ti saṅkhā samaññā paññatti vohāro nāmaṁ nāmakammaṁ nāmadheyyaṁ nirutti byañjanaṁ abhilāpoti—",
  "cnd21:8.10_0": "pārāyananteva adhivacanaṁ.",
  "cnd21:8.11_0": "Tenetaṁ vuccati—",
  "cnd21:9.1_0": "“Ekamekassa cepi pañhassa atthamaññāya dhammamaññāya dhammānudhammaṁ paṭipajjeyya,",
  "cnd21:9.1_1": "gaccheyyeva jarāmaraṇassa pāraṁ.",
  "cnd21:9.2_0": "Pāraṅgamanīyā ime dhammāti.",
  "cnd21:9.3_0": "Tasmā imassa dhammapariyāyassa ‘pārāyanan’teva adhivacanan”ti.",
  "cnd21:10.1_0": "<b>Ajito tissametteyyo,</b>",
  "cnd21:10.2_0": "<b>puṇṇako atha mettagū;</b>",
  "cnd21:10.3_0": "<b>Dhotako upasīvo ca,</b>",
  "cnd21:10.4_0": "<b>nando ca atha hemako. </b>",
  "cnd21:11.1_0": "<b>Todeyyakappā dubhayo,</b>",
  "cnd21:11.2_0": "<b>jatukaṇṇī ca paṇḍito;</b>",
  "cnd21:11.3_0": "<b>Bhadrāvudho udayo ca,</b>",
  "cnd21:11.4_0": "<b>posālo cāpi brāhmaṇo;</b>",
  "cnd21:11.5_0": "<b>Mogharājā ca medhāvī,</b>",
  "cnd21:11.6_0": "<b>piṅgiyo ca mahāisi. </b>",
  "cnd21:12.1_0": "<b>Ete buddhaṁ upāgacchuṁ,</b>",
  "cnd21:12.2_0": "<b>Sampannacaraṇaṁ isiṁ;</b>",
  "cnd21:12.3_0": "<b>Pucchantā nipuṇe pañhe,</b>",
  "cnd21:12.4_0": "<b>Buddhaseṭṭhaṁ upāgamuṁ. </b>",
  "cnd21:13.1_0": "<b>Ete buddhaṁ upāgacchun</b>ti.",
  "cnd21:13.2_0": "<b>Ete</b>ti soḷasa pārāyaniyā brāhmaṇā.",
  "cnd21:13.3_0": "<b>Buddho</b>ti yo so bhagavā sayambhū anācariyako pubbe ananussutesu dhammesu sāmaṁ saccāni abhisambujjhi,",
  "cnd21:13.3_1": "tattha ca sabbaññutaṁ patto balesu ca vasībhāvaṁ.",
  "cnd21:13.4_0": "<b>Buddho</b>ti kenaṭṭhena buddho?",
  "cnd21:13.5_0": "Bujjhitā saccānīti buddho,",
  "cnd21:13.5_1": "bodhetā pajāyāti buddho,",
  "cnd21:13.5_2": "sabbaññutāya buddho,",
  "cnd21:13.5_3": "sabbadassāvitāya buddho,",
  "cnd21:13.5_4": "abhiññeyyatāya buddho,",
  "cnd21:13.5_5": "visavitāya buddho,",
  "cnd21:13.5_6": "khīṇāsavasaṅkhātena buddho,",
  "cnd21:13.5_7": "nirupalepasaṅkhātena buddho,",
  "cnd21:13.5_8": "ekantavītarāgoti buddho,",
  "cnd21:13.5_9": "ekantavītadosoti buddho,",
  "cnd21:13.5_10": "ekantavītamohoti buddho,",
  "cnd21:13.5_11": "ekantanikkilesoti buddho,",
  "cnd21:13.5_12": "ekāyanamaggaṁ gatoti buddho,",
  "cnd21:13.5_13": "eko anuttaraṁ sammāsambodhiṁ abhisambuddhoti buddho,",
  "cnd21:13.5_14": "abuddhivihatattā buddhipaṭilābhāti buddho.",
  "cnd21:13.6_0": "<b>Buddho</b>ti netaṁ nāmaṁ mātarā kataṁ na pitarā kataṁ na bhātarā kataṁ na bhaginiyā kataṁ na mittāmaccehi",
  "cnd21:13.6_1": "kataṁ na ñātisālohitehi kataṁ na samaṇabrāhmaṇehi kataṁ na devatāhi kataṁ.",
  "cnd21:13.7_0": "Vimokkhantikametaṁ buddhānaṁ bhagavantānaṁ bodhiyā mūle saha sabbaññutañāṇassa paṭilābhā sacchikā paññatti,",
  "cnd21:13.7_1": "yadidaṁ buddhoti.",
  "cnd21:13.8_0": "<b>Ete buddhaṁ upāgacchun</b>ti ete buddhaṁ upāgamiṁsu upasaṅkamiṁsu payirupāsiṁsu paripucchiṁsu paripañhiṁsūti—",
  "cnd21:13.9_0": "ete buddhaṁ upāgacchuṁ.",
  "cnd21:14.1_0": "<b>Sampannacaraṇaṁ isin</b>ti <b>caraṇaṁ</b> vuccati sīlācāranibbatti.",
  "cnd21:14.2_0": "Sīlasaṁvaropi caraṇaṁ,",
  "cnd21:14.2_1": "indriyasaṁvaropi caraṇaṁ,",
  "cnd21:14.2_2": "bhojane mattaññutāpi caraṇaṁ,",
  "cnd21:14.2_3": "jāgariyānuyogopi caraṇaṁ,",
  "cnd21:14.2_4": "sattapi saddhammā caraṇaṁ,",
  "cnd21:14.2_5": "cattāripi jhānāni caraṇaṁ.",
  "cnd21:14.3_0": "<b>Sampannacaraṇan</b>ti sampannacaraṇaṁ seṭṭhacaraṇaṁ viseṭṭhacaraṇaṁ pāmokkhacaraṇaṁ uttamacaraṇaṁ pavaracaraṇaṁ.",
  "cnd21:14.4_0": "<b>Isī</b>ti isi bhagavā mahantaṁ sīlakkhandhaṁ esī gavesī pariyesīti isi …pe…",
  "cnd21:14.4_1": "mahesakkhehi vā sattehi esito gavesito pariyesito—",
  "cnd21:14.5_0": "“kahaṁ buddho, kahaṁ bhagavā, kahaṁ devadevo kahaṁ narāsabho”ti—",
  "cnd21:14.6_0": "isīti—",
  "cnd21:14.7_0": "sampannacaraṇaṁ isiṁ.",
  "cnd21:15.1_0": "<b>Pucchantā nipuṇe pañhe</b>ti.",
  "cnd21:15.2_0": "<b>Pucchantā</b>ti pucchantā yācantā ajjhesantā pasādentā.",
  "cnd21:15.3_0": "<b>Nipuṇe pañhe</b>ti gambhīre duddase duranubodhe sante paṇīte atakkāvacare nipuṇe paṇḍitavedanīye pañheti—",
  "cnd21:15.4_0": "pucchantā nipuṇe pañhe.",
  "cnd21:16.1_0": "<b>Buddhaseṭṭhaṁ upāgamun</b>ti.",
  "cnd21:16.2_0": "<b>Buddho</b>ti yo so bhagavā …pe… sacchikā paññatti, yadidaṁ buddhoti.",
  "cnd21:16.3_0": "<b>Seṭṭhan</b>ti aggaṁ seṭṭhaṁ viseṭṭhaṁ pāmokkhaṁ uttamaṁ pavaraṁ buddhaṁ upāgamuṁ upāgamiṁsu upasaṅkamiṁsu",
  "cnd21:16.3_1": "payirupāsiṁsu paripucchiṁsu paripañhiṁsūti—",
  "cnd21:16.4_0": "buddhaseṭṭhaṁ upāgamuṁ.",
  "cnd21:16.5_0": "Tenetaṁ vuccati—",
  "cnd21:17.1_0": "“Ete buddhaṁ upāgacchuṁ,",
  "cnd21:17.2_0": "sampannacaraṇaṁ isiṁ;",
  "cnd21:17.3_0": "Pucchantā nipuṇe pañhe,",
  "cnd21:17.4_0": "buddhaseṭṭhaṁ upāgamun”ti.",
  "cnd21:18.1_0": "<b>Tesaṁ buddho pabyākāsi,</b>",
  "cnd21:18.2_0": "<b>Pañhaṁ puṭṭho yathātathaṁ;</b>",
  "cnd21:18.3_0": "<b>Pañhānaṁ veyyākaraṇena,</b>",
  "cnd21:18.4_0": "<b>Tosesi brāhmaṇe muni. </b>",
  "cnd21:19.1_0": "<b>Tesaṁ buddho pabyākāsī</b>ti.",
  "cnd21:19.2_0": "<b>Tesan</b>ti soḷasānaṁ pārāyaniyānaṁ brāhmaṇānaṁ.",
  "cnd21:19.3_0": "<b>Buddho</b>ti yo so bhagavā …pe… sacchikā paññatti, yadidaṁ buddhoti.",
  "cnd21:19.4_0": "<b>Pabyākāsī</b>ti tesaṁ buddho pabyākāsi ācikkhi desesi paññapesi paṭṭhapesi vivari vibhaji uttānīakāsi pakāsesīti—",
  "cnd21:19.5_0": "tesaṁ buddho pabyākāsi.",
  "cnd21:20.1_0": "<b>Pañhaṁ puṭṭho yathātathan</b>ti.",
  "cnd21:20.2_0": "<b>Pañhaṁ puṭṭho</b>ti pañhaṁ puṭṭho pucchito yācito ajjhesito pasādito.",
  "cnd21:20.3_0": "<b>Yathātathan</b>ti yathā ācikkhitabbaṁ tathā ācikkhi,",
  "cnd21:20.3_1": "yathā desitabbaṁ tathā desesi,",
  "cnd21:20.3_2": "yathā paññapetabbaṁ tathā paññapesi,",
  "cnd21:20.3_3": "yathā paṭṭhapetabbaṁ tathā paṭṭhapesi,",
  "cnd21:20.3_4": "yathā vivaritabbaṁ tathā vivari,",
  "cnd21:20.3_5": "yathā vibhajitabbaṁ tathā vibhaji,",
  "cnd21:20.3_6": "yathā uttānīkātabbaṁ tathā uttānīakāsi,",
  "cnd21:20.3_7": "yathā pakāsitabbaṁ tathā pakāsesīti—",
  "cnd21:20.4_0": "pañhaṁ puṭṭho yathātathaṁ.",
  "cnd21:21.1_0": "<b>Pañhānaṁ veyyākaraṇenā</b>ti pañhānaṁ veyyākaraṇena ācikkhanena desanena paññapanena paṭṭhapanena",
  "cnd21:21.1_1": "vivaraṇena vibhajanena uttānīkammena pakāsanenāti—",
  "cnd21:21.2_0": "pañhānaṁ veyyākaraṇena.",
  "cnd21:22.1_0": "<b>Tosesi brāhmaṇe munī</b>ti.",
  "cnd21:22.2_0": "<b>Tosesī</b>ti tosesi vitosesi pasādesi ārādhesi attamane akāsi.",
  "cnd21:22.3_0": "<b>Brāhmaṇe</b>ti soḷasa pārāyaniye brāhmaṇe.",
  "cnd21:22.4_0": "<b>Munī</b>ti monaṁ vuccati ñāṇaṁ …pe… saṅgajālamaticca so munīti—",
  "cnd21:22.5_0": "tosesi brāhmaṇe muni.",
  "cnd21:22.6_0": "Tenetaṁ vuccati—",
  "cnd21:23.1_0": "“Tesaṁ buddho pabyākāsi,",
  "cnd21:23.2_0": "pañhaṁ puṭṭho yathātathaṁ;",
  "cnd21:23.3_0": "Pañhānaṁ veyyākaraṇena,",
  "cnd21:23.4_0": "tosesi brāhmaṇe munī”ti.",
  "cnd21:24.1_0": "<b>Te tositā cakkhumatā,</b>",
  "cnd21:24.2_0": "<b>buddhenādiccabandhunā;</b>",
  "cnd21:24.3_0": "<b>Brahmacariyamacariṁsu,</b>",
  "cnd21:24.4_0": "<b>varapaññassa santike. </b>",
  "cnd21:25.1_0": "<b>Te tositā cakkhumatā</b>ti.",
  "cnd21:25.2_0": "<b>Te</b>ti soḷasa pārāyaniyā brāhmaṇā.",
  "cnd21:25.3_0": "<b>Tositā</b>ti tositā vitositā pasāditā ārādhitā attamanā katāti—",
  "cnd21:25.4_0": "te tositā.",
  "cnd21:25.5_0": "<b>Cakkhumatā</b>ti bhagavā pañcahi cakkhūhi cakkhumā—",
  "cnd21:25.6_0": "maṁsacakkhunāpi cakkhumā,",
  "cnd21:25.6_1": "dibbacakkhunāpi cakkhumā,",
  "cnd21:25.6_2": "paññācakkhunāpi cakkhumā,",
  "cnd21:25.6_3": "buddhacakkhunāpi cakkhumā,",
  "cnd21:25.6_4": "samantacakkhunāpi cakkhumā.",
  "cnd21:25.7_0": "Kathaṁ bhagavā maṁsacakkhunāpi cakkhumā …pe…",
  "cnd21:25.8_0": "evaṁ bhagavā samantacakkhunāpi cakkhumāti—",
  "cnd21:25.9_0": "te tositā cakkhumatā.",
  "cnd21:26.1_0": "<b>Buddhenādiccabandhunā</b>ti.",
  "cnd21:26.2_0": "<b>Buddho</b>ti yo so bhagavā …pe… sacchikā paññatti, yadidaṁ buddhoti.",
  "cnd21:26.3_0": "<b>Ādiccabandhunā</b>ti <b>ādicco</b> vuccati sūriyo.",
  "cnd21:26.4_0": "So gotamo gottena, bhagavāpi gotamo gottena, bhagavā sūriyassa gottañātako gottabandhu.",
  "cnd21:26.5_0": "Tasmā buddho ādiccabandhūti—",
  "cnd21:26.6_0": "buddhenādiccabandhunā.",
  "cnd21:27.1_0": "<b>Brahmacariyamacariṁsū</b>ti brahmacariyaṁ vuccati asaddhammasamāpattiyā ārati virati paṭivirati veramaṇī",
  "cnd21:27.1_1": "viramaṇaṁ akiriyā akaraṇaṁ anajjhāpatti velāanatikkamo setughāto.",
  "cnd21:27.2_0": "Api ca nippariyāyavasena brahmacariyaṁ vuccati ariyo aṭṭhaṅgiko maggo, seyyathidaṁ—",
  "cnd21:27.3_0": "sammādiṭṭhi,",
  "cnd21:27.3_1": "sammāsaṅkappo,",
  "cnd21:27.3_2": "sammāvācā,",
  "cnd21:27.3_3": "sammākammanto,",
  "cnd21:27.3_4": "sammāājīvo,",
  "cnd21:27.3_5": "sammāvāyāmo,",
  "cnd21:27.3_6": "sammāsati,",
  "cnd21:27.3_7": "sammāsamādhi.",
  "cnd21:27.4_0": "<b>Brahmacariyamacariṁsū</b>ti brahmacariyaṁ cariṁsu acariṁsu samādāya vattiṁsūti—",
  "cnd21:27.5_0": "brahmacariyamacariṁsu.",
  "cnd21:28.1_0": "<b>Varapaññassa santike</b>ti varapaññassa aggapaññassa seṭṭhapaññassa viseṭṭhapaññassa pāmokkhapaññassa uttamapaññassa pavarapaññassa.",
  "cnd21:28.2_0": "<b>Santike</b>ti santike sāmantā āsanne avidūre upakaṭṭheti—",
  "cnd21:28.3_0": "varapaññassa santike.",
  "cnd21:28.4_0": "Tenetaṁ vuccati—",
  "cnd21:29.1_0": "“Te tositā cakkhumatā,",
  "cnd21:29.2_0": "buddhenādiccabandhunā;",
  "cnd21:29.3_0": "Brahmacariyamacariṁsu,",
  "cnd21:29.4_0": "varapaññassa santike”ti.",
  "cnd21:30.1_0": "<b>Ekamekassa pañhassa,</b>",
  "cnd21:30.2_0": "<b>yathā buddhena desitaṁ;</b>",
  "cnd21:30.3_0": "<b>Tathā yo paṭipajjeyya,</b>",
  "cnd21:30.4_0": "<b>gacche pāraṁ apārato. </b>",
  "cnd21:31.1_0": "<b>Ekamekassa pañhassā</b>ti ekamekassa ajitapañhassa, ekamekassa tissametteyyapañhassa …pe…",
  "cnd21:31.2_0": "ekamekassa piṅgiyapañhassāti—",
  "cnd21:31.3_0": "ekamekassa pañhassa.",
  "cnd21:32.1_0": "<b>Yathā buddhena desitan</b>ti.",
  "cnd21:32.2_0": "<b>Buddho</b>ti yo so bhagavā sayambhū …pe…",
  "cnd21:32.3_0": "sacchikā paññatti, yadidaṁ buddhoti.",
  "cnd21:32.4_0": "<b>Yathā buddhena desitan</b>ti yathā buddhena ācikkhitaṁ desitaṁ paññapitaṁ paṭṭhapitaṁ vivaritaṁ vibhajitaṁ uttānīkataṁ pakāsitanti—",
  "cnd21:32.5_0": "yathā buddhena desitaṁ.",
  "cnd21:33.1_0": "<b>Tathā yo paṭipajjeyyā</b>ti sammāpaṭipadaṁ anulomapaṭipadaṁ apaccanīkapaṭipadaṁ anvatthapaṭipadaṁ dhammānudhammapaṭipadaṁ paṭipajjeyyāti—",
  "cnd21:33.2_0": "tathā yo paṭipajjeyya.",
  "cnd21:34.1_0": "<b>Gacche pāraṁ apārato</b>ti pāraṁ vuccati amataṁ nibbānaṁ …pe… nirodho nibbānaṁ;",
  "cnd21:34.2_0": "apāraṁ vuccanti kilesā ca khandhā ca abhisaṅkhārā ca.",
  "cnd21:34.3_0": "<b>Gacche pāraṁ apārato</b>ti apārato pāraṁ gaccheyya,",
  "cnd21:34.3_1": "pāraṁ adhigaccheyya,",
  "cnd21:34.3_2": "pāraṁ phasseyya,",
  "cnd21:34.3_3": "pāraṁ sacchikareyyāti—",
  "cnd21:34.4_0": "gacche pāraṁ apārato.",
  "cnd21:34.5_0": "Tenetaṁ vuccati—",
  "cnd21:35.1_0": "“Ekamekassa pañhassa,",
  "cnd21:35.2_0": "yathā buddhena desitaṁ;",
  "cnd21:35.3_0": "Tathā yo paṭipajjeyya,",
  "cnd21:35.4_0": "gacche pāraṁ apārato”ti.",
  "cnd21:36.1_0": "<b>Apārā pāraṁ gaccheyya,</b>",
  "cnd21:36.2_0": "<b>bhāvento maggamuttamaṁ;</b>",
  "cnd21:36.3_0": "<b>Maggo so pāraṁ gamanāya,</b>",
  "cnd21:36.4_0": "<b>tasmā pārāyanaṁ iti. </b>",
  "cnd21:37.1_0": "<b>Apārā pāraṁ gaccheyyā</b>ti apāraṁ vuccanti kilesā ca khandhā ca abhisaṅkhārā ca;",
  "cnd21:37.2_0": "pāraṁ vuccati amataṁ nibbānaṁ …pe…",
  "cnd21:37.3_0": "taṇhakkhayo virāgo nirodho nibbānaṁ.",
  "cnd21:37.4_0": "<b>Apārā pāraṁ gaccheyyā</b>ti apārā pāraṁ gaccheyya,",
  "cnd21:37.4_1": "pāraṁ adhigaccheyya,",
  "cnd21:37.4_2": "pāraṁ phasseyya,",
  "cnd21:37.4_3": "pāraṁ sacchikareyyāti—",
  "cnd21:37.5_0": "apārā pāraṁ gaccheyya.",
  "cnd21:38.1_0": "<b>Bhāvento maggamuttaman</b>ti maggamuttamaṁ vuccati ariyo aṭṭhaṅgiko maggo, seyyathidaṁ—",
  "cnd21:38.2_0": "sammādiṭṭhi …pe… sammāsamādhi.",
  "cnd21:38.3_0": "<b>Maggamuttaman</b>ti maggaṁ aggaṁ seṭṭhaṁ viseṭṭhaṁ pāmokkhaṁ uttamaṁ pavaraṁ.",
  "cnd21:38.4_0": "<b>Bhāvento</b>ti bhāvento āsevanto bahulīkarontoti—",
  "cnd21:38.5_0": "bhāvento maggamuttamaṁ.",
  "cnd21:39.1_0": "<b>Maggo so pāraṁ gamanāyā</b>ti—",
  "cnd21:40.1_0": "Maggo pantho patho pajjo,",
  "cnd21:40.2_0": "añjasaṁ vaṭumāyanaṁ;",
  "cnd21:40.3_0": "Nāvā uttarasetu ca,",
  "cnd21:40.4_0": "kullo ca bhisi saṅkamo.",
  "cnd21:41.1_0": "<b>Pāraṁ gamanāyā</b>ti pāraṁ gamanāya pāraṁ sampāpanāya pāraṁ samanupāpanāya jarāmaraṇassa taraṇāyāti—",
  "cnd21:41.2_0": "maggo so pāraṁ gamanāya.",
  "cnd21:42.1_0": "<b>Tasmā pārāyanaṁ itī</b>ti.",
  "cnd21:42.2_0": "<b>Tasmā</b>ti tasmā taṅkāraṇā taṁhetu tappaccayā taṁnidānā.",
  "cnd21:42.3_0": "Pāraṁ vuccati amataṁ nibbānaṁ …pe… nirodho nibbānaṁ.",
  "cnd21:42.4_0": "Ayanaṁ vuccati maggo.",
  "cnd21:42.5_0": "<b>Itī</b>ti padasandhi …pe… padānupubbatāpetaṁ itīti—",
  "cnd21:42.6_0": "tasmā pārāyanaṁ iti.",
  "cnd21:42.7_0": "Tenetaṁ vuccati—",
  "cnd21:43.1_0": "“Apārā pāraṁ gaccheyya,",
  "cnd21:43.2_0": "bhāvento maggamuttamaṁ;",
  "cnd21:43.3_0": "Maggo so pāraṁ gamanāya,",
  "cnd21:43.4_0": "tasmā pārāyanaṁ itī”ti.",
  "cnd21:43.5_0": "Pārāyanatthutigāthāniddeso sattarasamo."
}