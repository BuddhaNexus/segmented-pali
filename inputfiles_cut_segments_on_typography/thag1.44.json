{
  "thag1.44:0.1_0": "Theragāthā 1.44",
  "thag1.44:0.2_0": "Ekakanipāta",
  "thag1.44:0.3_0": "Pañcamavagga",
  "thag1.44:0.4_0": "Sānuttheragāthā",
  "thag1.44:1.1_0": "“Mataṁ vā amma rodanti,",
  "thag1.44:1.2_0": "Yo vā jīvaṁ na dissati;",
  "thag1.44:1.3_0": "Jīvantaṁ maṁ amma passantī,",
  "thag1.44:1.4_0": "Kasmā maṁ amma rodasī”ti.",
  "thag1.44:1.5_0": "… Sānutthero …."
}