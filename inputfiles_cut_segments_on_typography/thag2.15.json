{
  "thag2.15:0.1_0": "Theragāthā 2.15",
  "thag2.15:0.2_0": "Dukanipāta",
  "thag2.15:0.3_0": "Dutiyavagga",
  "thag2.15:0.4_0": "Sabbamittattheragāthā",
  "thag2.15:1.1_0": "“Jano janamhi sambaddho,",
  "thag2.15:1.2_0": "janamevassito jano;",
  "thag2.15:1.3_0": "Jano janena heṭhīyati,",
  "thag2.15:1.4_0": "heṭheti ca jano janaṁ.",
  "thag2.15:2.1_0": "Ko hi tassa janenattho,",
  "thag2.15:2.2_0": "janena janitena vā;",
  "thag2.15:2.3_0": "Janaṁ ohāya gacchaṁ taṁ,",
  "thag2.15:2.4_0": "heṭhayitvā bahuṁ janan”ti.",
  "thag2.15:2.5_0": "… Sabbamitto thero …."
}