{
  "ya1.1.2:0.1_0": "1 Mūlayamaka",
  "ya1.1.2:0.2_0": "1.1 Uddesa",
  "ya1.1.2:0.3_0": "1.1.2. Hetuvārādiuddesa",
  "ya1.1.2:1.1_0": "Ye keci kusalā dhammā,",
  "ya1.1.2:1.1_1": "sabbe te kusalahetū …pe…",
  "ya1.1.2:1.1_2": "kusalanidānā …pe…",
  "ya1.1.2:1.1_3": "kusalasambhavā …pe…",
  "ya1.1.2:1.1_4": "kusalappabhavā …pe…",
  "ya1.1.2:1.1_5": "kusalasamuṭṭhānā …pe…",
  "ya1.1.2:1.1_6": "kusalāhārā …pe…",
  "ya1.1.2:1.1_7": "kusalārammaṇā …pe…",
  "ya1.1.2:1.1_8": "kusalapaccayā …pe…",
  "ya1.1.2:1.1_9": "kusalasamudayā …pe….",
  "ya1.1.2:2.1_0": "Mūlaṁ hetu nidānañca,",
  "ya1.1.2:2.2_0": "Sambhavo pabhavena ca;",
  "ya1.1.2:2.3_0": "Samuṭṭhānāhārārammaṇā,",
  "ya1.1.2:2.4_0": "Paccayo samudayena cāti.",
  "ya1.1.2:3.1_0": "Uddesavāro niṭṭhito."
}