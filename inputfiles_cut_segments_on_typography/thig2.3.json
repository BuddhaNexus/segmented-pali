{
  "thig2.3:0.1_0": "Therīgāthā 2.3",
  "thig2.3:0.2_0": "Dukanipāta",
  "thig2.3:0.3_0": "Sumaṅgalamātātherīgāthā",
  "thig2.3:1.1_0": "“Sumuttikā sumuttikā,",
  "thig2.3:1.2_0": "Sādhumuttikāmhi musalassa;",
  "thig2.3:1.3_0": "Ahiriko me chattakaṁ vāpi,",
  "thig2.3:1.4_0": "Ukkhalikā me deḍḍubhaṁ vāti.",
  "thig2.3:2.1_0": "Rāgañca ahaṁ dosañca,",
  "thig2.3:2.2_0": "Cicciṭi cicciṭīti vihanāmi;",
  "thig2.3:2.3_0": "Sā rukkhamūlamupagamma,",
  "thig2.3:2.4_0": "Aho sukhanti sukhato jhāyāmī”ti.",
  "thig2.3:3.1_0": "… Sumaṅgalamātā therī …."
}