{
  "tha-ap87:0.1_0": "Therāpadāna",
  "tha-ap87:0.2_0": "Timiravagga",
  "tha-ap87:0.3_0": "5. Raṁsisaññakattheraapadāna",
  "tha-ap87:1.1_0": "“Pabbate himavantamhi,",
  "tha-ap87:1.2_0": "vāsaṁ kappesahaṁ pure;",
  "tha-ap87:1.3_0": "Ajinuttaravāsohaṁ,",
  "tha-ap87:1.4_0": "vasāmi pabbatantare.",
  "tha-ap87:2.1_0": "Suvaṇṇavaṇṇaṁ sambuddhaṁ,",
  "tha-ap87:2.2_0": "Sataraṁsiṁva bhāṇumaṁ;",
  "tha-ap87:2.3_0": "Vanantaragataṁ disvā,",
  "tha-ap87:2.4_0": "Sālarājaṁva pupphitaṁ.",
  "tha-ap87:3.1_0": "Raṁsyā cittaṁ pasādetvā,",
  "tha-ap87:3.2_0": "vipassissa mahesino;",
  "tha-ap87:3.3_0": "Paggayha añjaliṁ vandiṁ,",
  "tha-ap87:3.4_0": "sirasā ukkuṭī ahaṁ.",
  "tha-ap87:4.1_0": "Ekanavutito kappe,",
  "tha-ap87:4.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap87:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap87:4.4_0": "raṁsisaññāyidaṁ phalaṁ.",
  "tha-ap87:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap87:5.2_0": "…pe…",
  "tha-ap87:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap87:6.1_0": "Itthaṁ sudaṁ āyasmā raṁsisaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap87:7.1_0": "Raṁsisaññakattherassāpadānaṁ pañcamaṁ."
}