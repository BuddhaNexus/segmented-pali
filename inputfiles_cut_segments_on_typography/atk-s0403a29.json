{
  "atk-s0403a29:0_0": "4. Devatāvaggo",
  "atk-s0403a29:1_0": "1. Sekhasuttavaṇṇanā",
  "atk-s0403a29:2_0": "31.",
  "atk-s0403a29:2_1": "Catutthassa paṭhame sekhassāti sattavidhassa sekhassa.",
  "atk-s0403a29:2_2": "Puthujjane pana vattabbameva natthi.",
  "atk-s0403a29:2_3": "Parihānāyāti uparūpariguṇaparihānāya.",
  "atk-s0403a29:3_0": "2-3. Aparihānasuttadvayavaṇṇanā",
  "atk-s0403a29:4_0": "32- 33.",
  "atk-s0403a29:4_1": "Dutiye satthugāravatāti satthari garubhāvo.",
  "atk-s0403a29:4_2": "Dhammagāravatāti navavidhe lokuttaradhamme garubhāvo.",
  "atk-s0403a29:4_3": "Saṅghagāravatāti saṅghe garubhāvo.",
  "atk-s0403a29:4_4": "Sikkhāgāravatāti tīsu sikkhāsu garubhāvo.",
  "atk-s0403a29:4_5": "Appamādagāravatāti appamāde garubhāvo.",
  "atk-s0403a29:4_6": "Paṭisanthāragāravatāti dhammāmisavasena duvidhe paṭisanthāre garubhāvo.",
  "atk-s0403a29:4_7": "Satthā garu assāti satthugaru.",
  "atk-s0403a29:4_8": "Dhammo garu assāti dhammagaru.",
  "atk-s0403a29:4_9": "Tibbagāravoti bahalagāravo.",
  "atk-s0403a29:4_10": "Paṭisanthāre gāravo assāti paṭisanthāragāravo.",
  "atk-s0403a29:4_11": "Tatiye sappatissoti sajeṭṭhako sagāravo.",
  "atk-s0403a29:4_12": "Hirottappaṁ panettha missakaṁ kathitaṁ.",
  "atk-s0403a29:5_0": "4. Mahāmoggallānasuttavaṇṇanā",
  "atk-s0403a29:6_0": "34.",
  "atk-s0403a29:6_1": "Catutthe tisso nāma bhikkhūti therasseva saddhivihāriko.",
  "atk-s0403a29:6_2": "Mahiddhiko mahānubhāvoti ijjhanaṭṭhena mahatī iddhi assāti mahiddhiko.",
  "atk-s0403a29:6_3": "Anupharaṇaṭṭhena mahā ānubhāvo assāti mahānubhāvo.",
  "atk-s0403a29:6_4": "Cirassaṁ kho,",
  "atk-s0403a29:6_5": "mārisa moggallāna,",
  "atk-s0403a29:6_6": "imaṁ pariyāyamakāsīti evarūpaṁ loke pakatiyā piyasamudāhāravacanaṁ hoti.",
  "atk-s0403a29:6_7": "Lokiyā hi cirassaṁ āgatampi anāgatapubbampi manāpajātiyaṁ āgataṁ disvā “kuto bhavaṁ āgato,",
  "atk-s0403a29:6_8": "cirassaṁ bhavaṁ āgato,",
  "atk-s0403a29:6_9": "kathaṁ te idhāgamanamaggo ñāto,",
  "atk-s0403a29:6_10": "kiṁ maggamūḷhosī”tiādīni vadanti.",
  "atk-s0403a29:6_11": "Ayaṁ pana āgatapubbattāyeva evamāha.",
  "atk-s0403a29:6_12": "Thero hi kālena kālaṁ brahmalokaṁ gacchatiyeva.",
  "atk-s0403a29:6_13": "Tattha pariyāyamakāsīti vāraṁ akāsi.",
  "atk-s0403a29:6_14": "Yadidaṁ idhāgamanāyāti yo ayaṁ idhāgamanāya vāro,",
  "atk-s0403a29:6_15": "taṁ cirassaṁ akāsīti vuttaṁ hoti.",
  "atk-s0403a29:6_16": "Idamāsanaṁ paññattanti mahārahaṁ brahmapallaṅkaṁ paññāpetvā evamāha.",
  "atk-s0403a29:6_17": "Aveccappasādenāti adhigatena acalena maggappasādena.",
  "atk-s0403a29:6_18": "Imasmiṁ sutte sotāpattimaggañāṇaṁ kathitaṁ.",
  "atk-s0403a29:7_0": "5. Vijjābhāgiyasuttavaṇṇanā",
  "atk-s0403a29:8_0": "35.",
  "atk-s0403a29:8_1": "Pañcame vijjābhāgiyāti vijjākoṭṭhāsikā.",
  "atk-s0403a29:8_2": "Aniccasaññāti aniccānupassanāñāṇe uppannasaññā.",
  "atk-s0403a29:8_3": "Anicce dukkhasaññāti dukkhānupassanāñāṇe uppannasaññā.",
  "atk-s0403a29:8_4": "Dukkhe anattasaññāti anattānupassanāñāṇe uppannasaññā.",
  "atk-s0403a29:8_5": "Pahānasaññāti pahānānupassanāñāṇe uppannasaññā.",
  "atk-s0403a29:8_6": "Virāgasaññāti virāgānupassanāñāṇe uppannasaññā.",
  "atk-s0403a29:8_7": "Nirodhasaññāti nirodhānupassanāñāṇe uppannasaññā.",
  "atk-s0403a29:9_0": "6. Vivādamūlasuttavaṇṇanā",
  "atk-s0403a29:10_0": "36.",
  "atk-s0403a29:10_1": "Chaṭṭhe vivādamūlānīti vivādassa mūlāni.",
  "atk-s0403a29:10_2": "Kodhanoti kujjhanalakkhaṇena kodhena samannāgato.",
  "atk-s0403a29:10_3": "Upanāhīti veraappaṭinissaggalakkhaṇena upanāhena samannāgato.",
  "atk-s0403a29:10_4": "Ahitāya dukkhāya devamanussānanti dvinnaṁ bhikkhūnaṁ vivādo kathaṁ devamanussānaṁ ahitāya dukkhāya saṁvattati?",
  "atk-s0403a29:10_5": "Kosambakakkhandhake viya dvīsu bhikkhūsu vivādaṁ āpannesu tasmiṁ vihāre tesaṁ antevāsikā vivadanti,",
  "atk-s0403a29:10_6": "tesaṁ ovādaṁ gaṇhanto bhikkhunisaṅgho vivadati.",
  "atk-s0403a29:10_7": "Tato tesaṁ upaṭṭhākā vivadanti,",
  "atk-s0403a29:10_8": "atha manussānaṁ ārakkhadevatā dve koṭṭhāsā honti.",
  "atk-s0403a29:10_9": "Tathā dhammavādīnaṁ ārakkhadevatā dhammavādiniyo honti,",
  "atk-s0403a29:10_10": "adhammavādīnaṁ adhammavādiniyo.",
  "atk-s0403a29:10_11": "Tato ārakkhadevatānaṁ mittā bhummadevatā bhijjanti.",
  "atk-s0403a29:10_12": "Evaṁ paramparāya yāva brahmalokā ṭhapetvā ariyasāvake sabbe devamanussā dve koṭṭhāsā honti.",
  "atk-s0403a29:10_13": "Dhammavādīhi pana adhammavādinova bahutarā honti.",
  "atk-s0403a29:10_14": "Tato yaṁ bahukehi gahitaṁ,",
  "atk-s0403a29:10_15": "taṁ gacchanti.",
  "atk-s0403a29:10_16": "Dhammaṁ vissajjetvā bahutarāva adhammaṁ gaṇhanti.",
  "atk-s0403a29:10_17": "Te adhammaṁ purakkhatvā viharantā apāye nibbattanti.",
  "atk-s0403a29:10_18": "Evaṁ dvinnaṁ bhikkhūnaṁ vivādo devamanussānaṁ ahitāya dukkhāya hoti.",
  "atk-s0403a29:10_19": "Ajjhattaṁ vāti tumhākaṁ abbhantaraparisāya.",
  "atk-s0403a29:10_20": "Bahiddhāti paresaṁ parisāya.",
  "atk-s0403a29:11_0": "Makkhīti paresaṁ guṇamakkhanalakkhaṇena makkhena samannāgato.",
  "atk-s0403a29:11_1": "Paḷāsīti yugaggāhalakkhaṇena paḷāsena samannāgato.",
  "atk-s0403a29:11_2": "Issukīti parassa sakkārādīni issāyanalakkhaṇāya issāya samannāgato.",
  "atk-s0403a29:11_3": "Maccharīti āvāsamacchariyādīhi samannāgato.",
  "atk-s0403a29:11_4": "Saṭhoti kerāṭiko.",
  "atk-s0403a29:11_5": "Māyāvīti katapaṭicchādako.",
  "atk-s0403a29:11_6": "Pāpicchoti asantasambhāvanicchako dussīlo.",
  "atk-s0403a29:11_7": "Micchādiṭṭhīti natthikavādī,",
  "atk-s0403a29:11_8": "ahetuvādī,",
  "atk-s0403a29:11_9": "akiriyavādī.",
  "atk-s0403a29:11_10": "Sandiṭṭhiparāmāsīti sayaṁ diṭṭhameva parāmasati.",
  "atk-s0403a29:11_11": "Ādhānaggāhīti daḷhaggāhī.",
  "atk-s0403a29:11_12": "Duppaṭinissaggīti na sakkā hoti gahitaṁ vissajjāpetuṁ.",
  "atk-s0403a29:11_13": "Imasmiṁ sutte vaṭṭameva kathitaṁ.",
  "atk-s0403a29:12_0": "7. Dānasuttavaṇṇanā",
  "atk-s0403a29:13_0": "37.",
  "atk-s0403a29:13_1": "Sattame veḷukaṇḍakīti veḷukaṇḍakanagaravāsinī.",
  "atk-s0403a29:13_2": "Chaḷaṅgasamannāgatanti chahi guṇaṅgehi samannāgataṁ.",
  "atk-s0403a29:13_3": "Dakkhiṇaṁ patiṭṭhāpetīti dānaṁ deti.",
  "atk-s0403a29:13_4": "Pubbeva dānā sumanoti dānaṁ dassāmīti māsaḍḍhamāsato paṭṭhāya somanassappatto hoti.",
  "atk-s0403a29:13_5": "Ettha hi pubbecetanā dassāmīti cittuppādakālato paṭṭhāya “ito uṭṭhitena dānaṁ dassāmī”ti khettaggahaṇaṁ ādiṁ katvā cintentassa labbhati.",
  "atk-s0403a29:13_6": "Dadaṁ cittaṁ pasādetīti evaṁ vuttā muñcacetanā pana dānakāleyeva labbhati.",
  "atk-s0403a29:13_7": "Datvā attamano hotīti ayaṁ pana aparacetanā aparāparaṁ anussarantassa labbhati.",
  "atk-s0403a29:13_8": "Vītarāgāti vigatarāgā khīṇāsavā.",
  "atk-s0403a29:13_9": "Rāgavinayāya vā paṭipannāti rāgavinayapaṭipadaṁ paṭipannā.",
  "atk-s0403a29:13_10": "Ukkaṭṭhadesanā cesā,",
  "atk-s0403a29:13_11": "na kevalaṁ pana khīṇāsavānaṁ, anāgāmi-",
  "atk-s0403a29:13_12": "sakadāgāmi-",
  "atk-s0403a29:13_13": "sotāpannānampi antamaso tadahupabbajitassa bhaṇḍagāhakasāmaṇerassāpi dinnā dakkhiṇā chaḷaṅgasamannāgatāva hoti.",
  "atk-s0403a29:13_14": "Sopi hi sotāpattimaggatthameva pabbajito.",
  "atk-s0403a29:14_0": "Yaññassa sampadāti dānassa paripuṇṇatā.",
  "atk-s0403a29:14_1": "Saññatāti sīlasaññamena saññatā.",
  "atk-s0403a29:14_2": "Sayaṁ ācamayitvānāti attanāva hatthapāde dhovitvā mukhaṁ vikkhāletvā.",
  "atk-s0403a29:14_3": "Sakehi pāṇibhīti attano hatthehi.",
  "atk-s0403a29:14_4": "Sayehītipi pāṭho.",
  "atk-s0403a29:14_5": "Saddhoti ratanattayaguṇe saddahanto.",
  "atk-s0403a29:14_6": "Muttena cetasāti lābhamacchariyādīhi vimuttena cittena.",
  "atk-s0403a29:14_7": "Abyāpajjhaṁsukhaṁ lokanti niddukkhaṁ uḷārasukhasomanassaṁ devalokaṁ.",
  "atk-s0403a29:15_0": "8. Attakārīsuttavaṇṇanā",
  "atk-s0403a29:16_0": "38.",
  "atk-s0403a29:16_1": "Aṭṭhame addasaṁ vā assosiṁ vāti akkhīni ummīletvā mā addasaṁ,",
  "atk-s0403a29:16_2": "asukasmiṁ nāma ṭhāne vasatīti mā assosiṁ,",
  "atk-s0403a29:16_3": "kathentassa vā vacanaṁ mā assosiṁ.",
  "atk-s0403a29:16_4": "Kathañhi nāmāti kena nāma kāraṇena.",
  "atk-s0403a29:16_5": "Ārambhadhātūti ārabhanavasena pavattavīriyaṁ.",
  "atk-s0403a29:16_6": "Nikkamadhātūti kosajjato nikkhamanasabhāvaṁ vīriyaṁ.",
  "atk-s0403a29:16_7": "Parakkamadhātūti parakkamasabhāvo.",
  "atk-s0403a29:16_8": "Thāmadhātūti thāmasabhāvo.",
  "atk-s0403a29:16_9": "Ṭhitidhātūti ṭhitisabhāvo.",
  "atk-s0403a29:16_10": "Upakkamadhātūti upakkamasabhāvo.",
  "atk-s0403a29:16_11": "Sabbaṁ cetaṁ tena tenākārena pavattassa vīriyasseva nāmaṁ.",
  "atk-s0403a29:17_0": "9-10. Nidānasuttādivaṇṇanā",
  "atk-s0403a29:18_0": "39- 40.",
  "atk-s0403a29:18_1": "Navame kammānanti vaṭṭagāmikammānaṁ.",
  "atk-s0403a29:18_2": "Samudayāyāti piṇḍakaraṇatthāya.",
  "atk-s0403a29:18_3": "Nidānanti paccayo.",
  "atk-s0403a29:18_4": "Lobhajenāti lobhato jātena.",
  "atk-s0403a29:18_5": "Napaññāyantīti “evarūpena kammena nibbattā”ti na dissanti.",
  "atk-s0403a29:18_6": "Sukkapakkhe kammānanti vivaṭṭagāmikammānaṁ.",
  "atk-s0403a29:18_7": "Iti imasmiṁ sutte vaṭṭavivaṭṭaṁ kathitaṁ.",
  "atk-s0403a29:18_8": "Dasame niculavaneti mahāmucalindavane.",
  "atk-s0403a29:18_9": "Saddhammoti sāsanasaddhammo.",
  "atk-s0403a29:19_0": "11. Dārukkhandhasuttavaṇṇanā",
  "atk-s0403a29:20_0": "41.",
  "atk-s0403a29:20_1": "Ekādasame cetovasippattoti cittavasibhāvaṁ patto.",
  "atk-s0403a29:20_2": "Pathavītveva adhimucceyyāti thaddhākāraṁ pathavīdhātūti sallakkheyya.",
  "atk-s0403a29:20_3": "Yaṁnissāyāti yaṁ vijjamānaṁ thaddhākāraṁ pathavīdhātuṁ nissāya amuṁ dārukkhandhaṁ pathavītveva adhimucceyya,",
  "atk-s0403a29:20_4": "sā ettha pathavīdhātu atthīti.",
  "atk-s0403a29:20_5": "Iminā nayena sesapadānipi veditabbāni.",
  "atk-s0403a29:20_6": "Yatheva hi tasmiṁ thaddhākārā pathavīdhātu atthi,",
  "atk-s0403a29:20_7": "evaṁ yūsākārā āpodhātu,",
  "atk-s0403a29:20_8": "uṇhākārā tejodhātu,",
  "atk-s0403a29:20_9": "vitthambhanākārā vāyodhātu,",
  "atk-s0403a29:20_10": "rattavaṇṇamhi sāre padumapupphavaṇṇā subhadhātu,",
  "atk-s0403a29:20_11": "pūtibhūte cuṇṇe ceva pheggupapaṭikāsu ca amanuññavaṇṇā asubhadhātu,",
  "atk-s0403a29:20_12": "taṁ nissāya amuṁ dārukkhandhaṁ asubhantveva adhimucceyya sallakkheyyāti.",
  "atk-s0403a29:20_13": "Imasmiṁ sutte missakavihāro nāma kathito.",
  "atk-s0403a29:21_0": "12. Nāgitasuttavaṇṇanā",
  "atk-s0403a29:22_0": "42.",
  "atk-s0403a29:22_1": "Dvādasame gāmantavihārinti gāmantasenāsanavāsiṁ.",
  "atk-s0403a29:22_2": "Samāhitaṁ nisinnanti tasmiṁ gāmantasenāsane samādhiṁ appetvā nisinnaṁ.",
  "atk-s0403a29:22_3": "Idānimanti idāni imaṁ.",
  "atk-s0403a29:22_4": "Samādhimhā cāvessatīti samādhito uṭṭhāpessati.",
  "atk-s0403a29:22_5": "Na attamano homīti na sakamano homi.",
  "atk-s0403a29:22_6": "Pacalāyamānanti niddāyamānaṁ.",
  "atk-s0403a29:22_7": "Ekattanti ekasabhāvaṁ,",
  "atk-s0403a29:22_8": "ekaggatābhūtaṁ araññasaññaṁyeva citte karissatīti attho.",
  "atk-s0403a29:22_9": "Anurakkhissatīti anuggaṇhissati.",
  "atk-s0403a29:22_10": "Avimuttaṁ vā cittaṁ vimocessatīti aññasmiṁ kāle avimuttaṁ cittaṁ idāni pañcahi vimuttīhi vimocayissati.",
  "atk-s0403a29:22_11": "Riñcatīti vajjeti vissajjeti.",
  "atk-s0403a29:22_12": "Paṭipaṇāmetvāti panuditvā vissajjetvā.",
  "atk-s0403a29:22_13": "Uccārapassāvakammāyāti uccārapassāvakaraṇatthāya.",
  "atk-s0403a29:22_14": "Iminā ettakena ṭhānena satthārā araññasenāsanassa vaṇṇo kathito.",
  "atk-s0403a29:22_15": "Suttassa pana paṭhamakoṭṭhāse yaṁ vattabbaṁ,",
  "atk-s0403a29:22_16": "taṁ heṭṭhā vuttamevāti.",
  "atk-s0403a29:23_0": "Devatāvaggo catuttho."
}