{
  "kv12.8:0.1_0": "Kathāvatthu",
  "kv12.8:0.2_0": "Mahāpaṇṇāsaka",
  "kv12.8:0.3_0": "Dvādasamavagga",
  "kv12.8:0.4_0": "Jīvitāvoropanakathā",
  "kv12.8:1.1_0": "Diṭṭhisampanno puggalo sañcicca pāṇaṁ jīvitā voropeyyāti?",
  "kv12.8:1.2_0": "Āmantā.",
  "kv12.8:1.3_0": "Diṭṭhisampanno puggalo sañcicca mātaraṁ jīvitā voropeyya …pe…",
  "kv12.8:1.4_0": "pitaraṁ jīvitā voropeyya …pe…",
  "kv12.8:1.5_0": "arahantaṁ jīvitā voropeyya …pe…",
  "kv12.8:1.6_0": "duṭṭhena cittena tathāgatassa lohitaṁ uppādeyya …pe…",
  "kv12.8:1.7_0": "saṅghaṁ bhindeyyāti?",
  "kv12.8:1.8_0": "Na hevaṁ vattabbe …pe….",
  "kv12.8:2.1_0": "Diṭṭhisampanno puggalo sañcicca pāṇaṁ jīvitā voropeyyāti?",
  "kv12.8:2.2_0": "Āmantā.",
  "kv12.8:2.3_0": "Diṭṭhisampanno puggalo satthari agāravoti?",
  "kv12.8:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv12.8:2.5_0": "dhamme …pe…",
  "kv12.8:2.6_0": "saṅghe …pe…",
  "kv12.8:2.7_0": "sikkhāya agāravoti?",
  "kv12.8:2.8_0": "Na hevaṁ vattabbe …pe….",
  "kv12.8:3.1_0": "Nanu diṭṭhisampanno puggalo satthari sagāravoti?",
  "kv12.8:3.2_0": "Āmantā.",
  "kv12.8:3.3_0": "Hañci diṭṭhisampanno puggalo satthari sagāravo, no ca vata re vattabbe—",
  "kv12.8:3.4_0": "“diṭṭhisampanno puggalo sañcicca pāṇaṁ jīvitā voropeyyā”ti.",
  "kv12.8:3.5_0": "Nanu diṭṭhisampanno puggalo dhamme …pe…",
  "kv12.8:3.6_0": "saṅghe …pe…",
  "kv12.8:3.7_0": "sikkhāya sagāravoti?",
  "kv12.8:3.8_0": "Āmantā.",
  "kv12.8:3.9_0": "Hañci diṭṭhisampanno puggalo sikkhāya sagāravo, no ca vata re vattabbe—",
  "kv12.8:3.10_0": "“diṭṭhisampanno puggalo sañcicca pāṇaṁ jīvitā voropeyyā”ti.",
  "kv12.8:4.1_0": "Diṭṭhisampanno puggalo satthari agāravoti?",
  "kv12.8:4.2_0": "Āmantā.",
  "kv12.8:4.3_0": "Diṭṭhisampanno puggalo buddhathūpe ohadeyya omutteyya niṭṭhubheyya buddhathūpe apabyāmato kareyyāti?",
  "kv12.8:4.4_0": "Na hevaṁ vattabbe …pe….",
  "kv12.8:5.1_0": "Diṭṭhisampanno puggalo sañcicca pāṇaṁ jīvitā voropeyyāti?",
  "kv12.8:5.2_0": "Āmantā.",
  "kv12.8:5.3_0": "Nanu vuttaṁ bhagavatā—",
  "kv12.8:5.4_0": "“seyyathāpi, bhikkhave, mahāsamuddo ṭhitadhammo velaṁ nātivattati;",
  "kv12.8:5.5_0": "evameva kho,",
  "kv12.8:5.5_1": "bhikkhave,",
  "kv12.8:5.5_2": "yaṁ mayā sāvakānaṁ sikkhāpadaṁ paññattaṁ taṁ mama sāvakā jīvitahetupi nātikkamantī”ti.",
  "kv12.8:5.6_0": "Attheva suttantoti?",
  "kv12.8:5.7_0": "Āmantā.",
  "kv12.8:5.8_0": "Tena hi na vattabbaṁ—",
  "kv12.8:5.9_0": "“diṭṭhisampanno puggalo sañcicca pāṇaṁ jīvitā voropeyyā”ti.",
  "kv12.8:5.10_0": "Jīvitā voropanakathā niṭṭhitā."
}