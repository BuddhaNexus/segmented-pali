{
  "kv5.2:0.1_0": "Kathāvatthu",
  "kv5.2:0.2_0": "Mahāpaṇṇāsaka",
  "kv5.2:0.3_0": "Pañcamavagga",
  "kv5.2:0.4_0": "Asekhañāṇakathā",
  "kv5.2:1.1_0": "Sekhassa asekhaṁ ñāṇaṁ atthīti?",
  "kv5.2:1.2_0": "Āmantā.",
  "kv5.2:1.3_0": "Sekho asekhaṁ dhammaṁ jānāti passati,",
  "kv5.2:1.3_1": "diṭṭhaṁ viditaṁ sacchikataṁ upasampajja viharati,",
  "kv5.2:1.3_2": "kāyena phusitvā viharatīti?",
  "kv5.2:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv5.2:1.5_0": "nanu sekho asekhaṁ dhammaṁ na jānāti na passati,",
  "kv5.2:1.5_1": "adiṭṭhaṁ aviditaṁ asacchikataṁ na upasampajja viharati,",
  "kv5.2:1.5_2": "na kāyena phusitvā viharatīti?",
  "kv5.2:1.6_0": "Āmantā.",
  "kv5.2:1.7_0": "Hañci sekho asekhaṁ dhammaṁ na jānāti na passati,",
  "kv5.2:1.7_1": "adiṭṭhaṁ aviditaṁ asacchikataṁ na upasampajja viharati,",
  "kv5.2:1.7_2": "na kāyena phusitvā viharati,",
  "kv5.2:1.7_3": "no ca vata re vattabbe—",
  "kv5.2:1.8_0": "“sekhassa asekhaṁ ñāṇaṁ atthī”ti.",
  "kv5.2:2.1_0": "Asekhassa asekhaṁ ñāṇaṁ atthi,",
  "kv5.2:2.1_1": "asekho asekhaṁ dhammaṁ jānāti passati,",
  "kv5.2:2.1_2": "diṭṭhaṁ viditaṁ sacchikataṁ upasampajja viharati,",
  "kv5.2:2.1_3": "kāyena phusitvā viharatīti?",
  "kv5.2:2.2_0": "Āmantā.",
  "kv5.2:2.3_0": "Sekhassa asekhaṁ ñāṇaṁ atthi,",
  "kv5.2:2.3_1": "sekho asekhaṁ dhammaṁ jānāti passati,",
  "kv5.2:2.3_2": "diṭṭhaṁ viditaṁ sacchikataṁ upasampajja viharati,",
  "kv5.2:2.3_3": "kāyena phusitvā viharatīti?",
  "kv5.2:2.4_0": "Na hevaṁ vattabbe …pe….",
  "kv5.2:3.1_0": "Sekhassa asekhaṁ ñāṇaṁ atthi,",
  "kv5.2:3.1_1": "sekho asekhaṁ dhammaṁ na jānāti na passati,",
  "kv5.2:3.1_2": "adiṭṭhaṁ aviditaṁ asacchikataṁ na upasampajja viharati,",
  "kv5.2:3.1_3": "na kāyena phusitvā viharatīti?",
  "kv5.2:3.2_0": "Āmantā.",
  "kv5.2:3.3_0": "Asekhassa asekhaṁ ñāṇaṁ atthi,",
  "kv5.2:3.3_1": "asekho asekhaṁ dhammaṁ na jānāti na passati,",
  "kv5.2:3.3_2": "adiṭṭhaṁ aviditaṁ asacchikataṁ na upasampajja viharati,",
  "kv5.2:3.3_3": "na kāyena phusitvā viharatīti?",
  "kv5.2:3.4_0": "Na hevaṁ vattabbe …pe….",
  "kv5.2:4.1_0": "Sekhassa asekhaṁ ñāṇaṁ atthīti?",
  "kv5.2:4.2_0": "Āmantā.",
  "kv5.2:4.3_0": "Gotrabhuno puggalassa sotāpattimagge ñāṇaṁ atthīti?",
  "kv5.2:4.4_0": "Na hevaṁ vattabbe …pe…",
  "kv5.2:4.5_0": "sotāpattiphalasacchikiriyāya paṭipannassa puggalassa sotāpattiphale ñāṇaṁ atthīti?",
  "kv5.2:4.6_0": "Na hevaṁ vattabbe …pe…",
  "kv5.2:4.7_0": "sakadāgāmiphala …",
  "kv5.2:4.7_1": "anāgāmiphala …",
  "kv5.2:4.7_2": "arahattasacchikiriyāya paṭipannassa puggalassa arahatte ñāṇaṁ atthīti?",
  "kv5.2:4.8_0": "Na hevaṁ vattabbe …pe….",
  "kv5.2:5.1_0": "Na vattabbaṁ—",
  "kv5.2:5.2_0": "“sekhassa asekhaṁ ñāṇaṁ atthī”ti?",
  "kv5.2:5.3_0": "Āmantā.",
  "kv5.2:5.4_0": "Nanu āyasmā ānando sekho—“bhagavā uḷāro”ti jānāti,",
  "kv5.2:5.4_1": "“sāriputto thero,",
  "kv5.2:5.4_2": "mahāmoggallāno thero uḷāro”ti jānātīti?",
  "kv5.2:5.5_0": "Āmantā.",
  "kv5.2:5.6_0": "Hañci āyasmā ānando sekho—“bhagavā uḷāro”ti jānāti,",
  "kv5.2:5.6_1": "“sāriputto thero,",
  "kv5.2:5.6_2": "mahāmoggallāno thero uḷāro”ti jānāti,",
  "kv5.2:5.6_3": "tena vata re vattabbe—",
  "kv5.2:5.7_0": "“sekhassa asekhaṁ ñāṇaṁ atthī”ti.",
  "kv5.2:5.8_0": "Asekhañāṇakathā niṭṭhitā."
}