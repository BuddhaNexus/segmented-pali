{
  "ja453:0.1_0": "Jātaka",
  "ja453:0.2_0": "Dasakanipāta",
  "ja453:0.3_0": "Catudvāravagga",
  "ja453:0.4_0": "15. Mahāmaṅgalajātaka",
  "ja453:1.1_0": "“Kiṁsu naro jappamadhiccakāle,",
  "ja453:1.2_0": "Kaṁ vā vijjaṁ katamaṁ vā sutānaṁ;",
  "ja453:1.3_0": "So macco asmiñca paramhi loke,",
  "ja453:1.4_0": "Kathaṁ karo sotthānena gutto”.",
  "ja453:2.1_0": "“Yassa devā pitaro ca sabbe,",
  "ja453:2.2_0": "Sarīsapā sabbabhūtāni cāpi;",
  "ja453:2.3_0": "Mettāya niccaṁ apacitāni honti,",
  "ja453:2.4_0": "Bhūtesu ve sotthānaṁ tadāhu”.",
  "ja453:3.1_0": "“Yo sabbalokassa nivātavutti,",
  "ja453:3.2_0": "Itthīpumānaṁ sahadārakānaṁ;",
  "ja453:3.3_0": "Khantā duruttānamappaṭikūlavādī,",
  "ja453:3.4_0": "Adhivāsanaṁ sotthānaṁ tadāhu.",
  "ja453:4.1_0": "Yo nāvajānāti sahāyamitte,",
  "ja453:4.2_0": "Sippena kulyāhi dhanena jaccā;",
  "ja453:4.3_0": "Rucipañño atthakāle matīmā,",
  "ja453:4.4_0": "Sahāyesu ve sotthānaṁ tadāhu.",
  "ja453:5.1_0": "Mittāni ve yassa bhavanti santo,",
  "ja453:5.2_0": "Saṁvissatthā avisaṁvādakassa;",
  "ja453:5.3_0": "Na mittadubbhī saṁvibhāgī dhanena,",
  "ja453:5.4_0": "Mittesu ve sotthānaṁ tadāhu.",
  "ja453:6.1_0": "Yassa bhariyā tulyavayā samaggā,",
  "ja453:6.2_0": "Anubbatā dhammakāmā pajātā;",
  "ja453:6.3_0": "Koliniyā sīlavatī patibbatā,",
  "ja453:6.4_0": "Dāresu ve sotthānaṁ tadāhu.",
  "ja453:7.1_0": "Yassa rājā bhūtapati yasassī,",
  "ja453:7.2_0": "Jānāti soceyyaṁ parakkamañca;",
  "ja453:7.3_0": "Advejjhatā suhadayaṁ mamanti,",
  "ja453:7.4_0": "Rājūsu ve sotthānaṁ tadāhu.",
  "ja453:8.1_0": "Annañca pānañca dadāti saddho,",
  "ja453:8.2_0": "Mālañca gandhañca vilepanañca;",
  "ja453:8.3_0": "Pasannacitto anumodamāno,",
  "ja453:8.4_0": "Saggesu ve sotthānaṁ tadāhu.",
  "ja453:9.1_0": "Yamariyadhammena punanti vuddhā,",
  "ja453:9.2_0": "Ārādhitā samacariyāya santo;",
  "ja453:9.3_0": "Bahussutā isayo sīlavanto,",
  "ja453:9.4_0": "Arahantamajjhe sotthānaṁ tadāhu.",
  "ja453:10.1_0": "Etāni kho sotthānāni loke,",
  "ja453:10.2_0": "Viññuppasatthāni sukhudrayāni;",
  "ja453:10.3_0": "Tānīdha sevetha naro sapañño,",
  "ja453:10.4_0": "Na hi maṅgale kiñcanamatthi saccan”ti.",
  "ja453:11.1_0": "Mahāmaṅgalajātakaṁ pannarasamaṁ."
}