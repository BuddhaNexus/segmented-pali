{
  "thag2.39:0.1_0": "Theragāthā 2.39",
  "thag2.39:0.2_0": "Dukanipāta",
  "thag2.39:0.3_0": "Catutthavagga",
  "thag2.39:0.4_0": "Usabhattheragāthā",
  "thag2.39:1.1_0": "“Ambapallavasaṅkāsaṁ,",
  "thag2.39:1.2_0": "aṁse katvāna cīvaraṁ;",
  "thag2.39:1.3_0": "Nisinno hatthigīvāyaṁ,",
  "thag2.39:1.4_0": "gāmaṁ piṇḍāya pāvisiṁ.",
  "thag2.39:2.1_0": "Hatthikkhandhato oruyha,",
  "thag2.39:2.2_0": "saṁvegaṁ alabhiṁ tadā;",
  "thag2.39:2.3_0": "Sohaṁ ditto tadā santo,",
  "thag2.39:2.4_0": "patto me āsavakkhayo”ti.",
  "thag2.39:2.5_0": "… Usabho thero …."
}