{
  "anya-e0604n0:0_0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e0604n0:1_0": "Suttavandanā",
  "anya-e0604n0:2_0": "1.",
  "anya-e0604n0:3_0": "Dhammattha desanā vedha,",
  "anya-e0604n0:4_0": "Catugambhīra duddasaṃ;",
  "anya-e0604n0:5_0": "Dhammaṃ lokassa desentaṃ,",
  "anya-e0604n0:6_0": "Dhammarājaṃ namāmahaṃ;",
  "anya-e0604n0:7_0": "2.",
  "anya-e0604n0:8_0": "Buddho paccekabuddho ca,",
  "anya-e0604n0:9_0": "Arahā cakkavattīti;",
  "anya-e0604n0:10_0": "Thūpāthūpārahaṃ vande,",
  "anya-e0604n0:11_0": "Catuthūpārahesu taṃ;",
  "anya-e0604n0:12_0": "3.",
  "anya-e0604n0:13_0": "Petaseyyo kāmabhogī,",
  "anya-e0604n0:14_0": "Sīhaseyyo tathāgato;",
  "anya-e0604n0:15_0": "Sayantaṃ catuseyyesu,",
  "anya-e0604n0:16_0": "Catutthena name jinaṃ;",
  "anya-e0604n0:17_0": "4.",
  "anya-e0604n0:18_0": "Aṇḍajā jalābujā ca;",
  "anya-e0604n0:19_0": "Saṃsedajo papātikā;",
  "anya-e0604n0:20_0": "Catuyoni pariccheda,",
  "anya-e0604n0:21_0": "Ñāṇavantaṃ name jinaṃ;",
  "anya-e0604n0:22_0": "5.",
  "anya-e0604n0:23_0": "Sutabuddhonubuddho ca,",
  "anya-e0604n0:24_0": "Pacceko jinabuddhoti;",
  "anya-e0604n0:25_0": "Catutthaṃ catubuddhesu,",
  "anya-e0604n0:26_0": "Vandāmi lokanāyakaṃ;",
  "anya-e0604n0:27_0": "6.",
  "anya-e0604n0:28_0": "Rāhuggaṃ attabhāgīnaṃ,",
  "anya-e0604n0:29_0": "Mandhātā kāmabhogīnaṃ;",
  "anya-e0604n0:30_0": "Māro adhipateyyānaṃ,",
  "anya-e0604n0:31_0": "Buddho loke sadevake;",
  "anya-e0604n0:32_0": "Catuagga paññattīnaṃ,",
  "anya-e0604n0:33_0": "Aggapattaṃ name jinaṃ.",
  "anya-e0604n0:34_0": "7.",
  "anya-e0604n0:35_0": "Dānaṃ piyakathā attha,",
  "anya-e0604n0:36_0": "Cariyā sadisattatā;",
  "anya-e0604n0:37_0": "Catusaṅgaha vatthūhi,",
  "anya-e0604n0:38_0": "Asaṅkhiyesu jātisu;",
  "anya-e0604n0:39_0": "Lokassa saṅgahetāraṃ,",
  "anya-e0604n0:40_0": "Vandāmi lokanāyakaṃ;",
  "anya-e0604n0:41_0": "8.",
  "anya-e0604n0:42_0": "Lobho doso ca mohoti,",
  "anya-e0604n0:43_0": "Anto paccatthike tayo;",
  "anya-e0604n0:44_0": "Dujjaye sabbaverīhi,",
  "anya-e0604n0:45_0": "Sañjitaṃpi sudhiṃ name;",
  "anya-e0604n0:46_0": "9.",
  "anya-e0604n0:47_0": "Jāti puñña mahattāni,",
  "anya-e0604n0:48_0": "Guṇamahattamuttamaṃ;",
  "anya-e0604n0:49_0": "Timahatthehi sampannaṃ,",
  "anya-e0604n0:50_0": "Vandāmi lokanāyakaṃ;",
  "anya-e0604n0:51_0": "10.",
  "anya-e0604n0:52_0": "Hetu phala sampadāyo,",
  "anya-e0604n0:53_0": "Sattupakārasampadā;",
  "anya-e0604n0:54_0": "Tisampadāhi sampannaṃ,",
  "anya-e0604n0:55_0": "Vandāmi lokanāyakaṃ.",
  "anya-e0604n0:56_0": "11.",
  "anya-e0604n0:57_0": "Ñāṇappahānānubhāva,",
  "anya-e0604n0:58_0": "Surūpakārasampadā;",
  "anya-e0604n0:59_0": "Catuphala sampadāhi,",
  "anya-e0604n0:60_0": "Taṃ sampannaṃ name jinaṃ.",
  "anya-e0604n0:61_0": "12.",
  "anya-e0604n0:62_0": "Āsayo ca payogoti,",
  "anya-e0604n0:63_0": "Sattupakāra sampadā;",
  "anya-e0604n0:64_0": "Duvidhā yassa sampannā,",
  "anya-e0604n0:65_0": "Sammā maṃ pātu so jino.",
  "anya-e0604n0:66_0": "13.",
  "anya-e0604n0:67_0": "Mahosadhe vessantare,",
  "anya-e0604n0:68_0": "Pacchimabhavike tathā;",
  "anya-e0604n0:69_0": "Tijātīsu name vācaṃ,",
  "anya-e0604n0:70_0": "Bhāsentaṃ jāyanakkhaṇe.",
  "anya-e0604n0:71_0": "14.",
  "anya-e0604n0:72_0": "Nemimhi sādhine ceva,",
  "anya-e0604n0:73_0": "Mandhātari ca guttile;",
  "anya-e0604n0:74_0": "Catūsu manujattena,",
  "anya-e0604n0:75_0": "Devalokaṃ gataṃ name.",
  "anya-e0604n0:76_0": "15.",
  "anya-e0604n0:77_0": "Saṅkhāro rūpavikāro,",
  "anya-e0604n0:78_0": "Lakkhaṇaṃ vānanissaṭaṃ;",
  "anya-e0604n0:79_0": "Nibbānaṃ paññatticeti,",
  "anya-e0604n0:80_0": "Pañca ñeyyantaguṃ name.",
  "anya-e0604n0:81_0": "16.",
  "anya-e0604n0:82_0": "Putta dāra pariccāgā,",
  "anya-e0604n0:83_0": "Rajjaṅgaṃ jīva cāgīti;",
  "anya-e0604n0:84_0": "Pañca mahāpariccāge,",
  "anya-e0604n0:85_0": "Karaṃ sudukkare name.",
  "anya-e0604n0:86_0": "17.",
  "anya-e0604n0:87_0": "Rūpañca vedanā saññā,",
  "anya-e0604n0:88_0": "Saṅkhārā viññāṇantime;",
  "anya-e0604n0:89_0": "Pañcakkhandhe parijānaṃ,",
  "anya-e0604n0:90_0": "Tipariññāhi taṃ name.",
  "anya-e0604n0:91_0": "18.",
  "anya-e0604n0:92_0": "Manussa deva gatiyo,",
  "anya-e0604n0:93_0": "Peto ca nirayo tathā;",
  "anya-e0604n0:94_0": "Tiracchānoti pañcetā,",
  "anya-e0604n0:95_0": "Chindantaṃ gatiyo name;",
  "anya-e0604n0:96_0": "19.",
  "anya-e0604n0:97_0": "Devaputto kileso ca,",
  "anya-e0604n0:98_0": "Abhisaṅkhāra mārako;",
  "anya-e0604n0:99_0": "Khandho maccūti pañcete,",
  "anya-e0604n0:100_0": "Māre vijitavaṃ name.",
  "anya-e0604n0:101_0": "20.",
  "anya-e0604n0:102_0": "Purebhattaṃ pacchābhattaṃ,",
  "anya-e0604n0:103_0": "Purimañceva majjhimaṃ;",
  "anya-e0604n0:104_0": "Pacchimanti sadā pañca,",
  "anya-e0604n0:105_0": "Buddhakiccaṃ karaṃ name.",
  "anya-e0604n0:106_0": "21.",
  "anya-e0604n0:107_0": "Rūpā saddā gandhā rasā,",
  "anya-e0604n0:108_0": "Phoṭṭhabbā ca manoramā;",
  "anya-e0604n0:109_0": "Pañca kāmaguṇe hitvā,",
  "anya-e0604n0:110_0": "Kheḷaṃva niggataṃ name.",
  "anya-e0604n0:111_0": "22.",
  "anya-e0604n0:112_0": "Rāgasallaṃ dosasallaṃ,",
  "anya-e0604n0:113_0": "Mohā māno ca diṭṭhīti;",
  "anya-e0604n0:114_0": "Vande vajjātivajjantaṃ,",
  "anya-e0604n0:115_0": "Pañcasallapanūdanaṃ.",
  "anya-e0604n0:116_0": "23.",
  "anya-e0604n0:117_0": "Dīgho majjhima saṃyuttā,",
  "anya-e0604n0:118_0": "Aṅguttaro ca khuddako;",
  "anya-e0604n0:119_0": "Iti pañca nikāyehi,",
  "anya-e0604n0:120_0": "Suvinentaṃ pajaṃ name.",
  "anya-e0604n0:121_0": "24.",
  "anya-e0604n0:122_0": "Issariyaṃ yaso dhammo,",
  "anya-e0604n0:123_0": "Kāmo sirī payattīti;",
  "anya-e0604n0:124_0": "Bhagyehi chahi sampannaṃ,",
  "anya-e0604n0:125_0": "Vandāmi lokanāyakaṃ.",
  "anya-e0604n0:126_0": "25.",
  "anya-e0604n0:127_0": "Piyāpiyaṃ bhūtābhūtaṃ,",
  "anya-e0604n0:128_0": "Atthānatthaṃ yathārahaṃ;",
  "anya-e0604n0:129_0": "Missā chasu suvācaṃva,",
  "anya-e0604n0:130_0": "Bhāsantaṃ atthakaṃ name.",
  "anya-e0604n0:131_0": "26.",
  "anya-e0604n0:132_0": "Alaso ca pamādo ca,",
  "anya-e0604n0:133_0": "Anuṭṭhānaṃ asaṃyamo;",
  "anya-e0604n0:134_0": "Niddā tantīti chiddehi,",
  "anya-e0604n0:135_0": "Chahi muttaṃ sadā name.",
  "anya-e0604n0:136_0": "27.",
  "anya-e0604n0:137_0": "Mahākaruṇāsamāpatti,",
  "anya-e0604n0:138_0": "Yamakappāṭihāriyaṃ;",
  "anya-e0604n0:139_0": "Āsayānusaye ñāṇaṃ,",
  "anya-e0604n0:140_0": "Indriyāna paropare;",
  "anya-e0604n0:141_0": "Sabbaññutā nāvaraṇaṃ,",
  "anya-e0604n0:142_0": "Chā sādhāraṇikaṃ name.",
  "anya-e0604n0:143_0": "28.",
  "anya-e0604n0:144_0": "Kāmacchando ca byāpādo,",
  "anya-e0604n0:145_0": "Thinamiddhañca saṃsayo;",
  "anya-e0604n0:146_0": "Avijjuddhacca kukkuccaṃ,",
  "anya-e0604n0:147_0": "Chavinīvaraṇaṃ name.",
  "anya-e0604n0:148_0": "29.",
  "anya-e0604n0:149_0": "Iddhividhaṃ dibbasotaṃ,",
  "anya-e0604n0:150_0": "Paracitta vijānanā;",
  "anya-e0604n0:151_0": "Pubbenivāsānussati,",
  "anya-e0604n0:152_0": "Dibbacakkhāsavakkhayo;",
  "anya-e0604n0:153_0": "Chaḷabhiññāhi sampannaṃ,",
  "anya-e0604n0:154_0": "Vandāmi purisuttamaṃ.",
  "anya-e0604n0:155_0": "30.",
  "anya-e0604n0:156_0": "Nīla pītā ca odātā,",
  "anya-e0604n0:157_0": "Mañjiṭṭhā ca pabhassarā;",
  "anya-e0604n0:158_0": "Lohitāti cha raṃsīhī,",
  "anya-e0604n0:159_0": "Vijjotantaṃ sadā name.",
  "anya-e0604n0:160_0": "31.",
  "anya-e0604n0:161_0": "Assādādīnavo ceva,",
  "anya-e0604n0:162_0": "Nissaraṇaphalaṃ tathā;",
  "anya-e0604n0:163_0": "Upāyāṇattīti chadhā,",
  "anya-e0604n0:164_0": "Saddhammadesakaṃ name.",
  "anya-e0604n0:165_0": "32.",
  "anya-e0604n0:166_0": "Rāgo doso ca moho ca,",
  "anya-e0604n0:167_0": "Vitakkacariyā tathā;",
  "anya-e0604n0:168_0": "Saddhā buddhīti bhājentaṃ,",
  "anya-e0604n0:169_0": "Tā chaḷecariyā name.",
  "anya-e0604n0:170_0": "33.",
  "anya-e0604n0:171_0": "Rajataṃ kanakaṃ muttā,",
  "anya-e0604n0:172_0": "Maṇi veḷuriyāni ca;",
  "anya-e0604n0:173_0": "Vajirañca pavāḷanti,",
  "anya-e0604n0:174_0": "Seṭṭhi sattadhano viya.",
  "anya-e0604n0:175_0": "34.",
  "anya-e0604n0:176_0": "Saddhāsīlaṃ sutaṃcāgo,",
  "anya-e0604n0:177_0": "Hirī ottappiyaṃ dhanaṃ,",
  "anya-e0604n0:178_0": "Paññādhananti sambuddhaṃ,",
  "anya-e0604n0:179_0": "Vande sattamahādhanaṃ.",
  "anya-e0604n0:180_0": "35.",
  "anya-e0604n0:181_0": "Cakkaṃ hatthi asso maṇi,",
  "anya-e0604n0:182_0": "Gahapatitthiyo tathā;",
  "anya-e0604n0:183_0": "Supariṇāyakocāti,",
  "anya-e0604n0:184_0": "Sattaratano cakkavattīva.",
  "anya-e0604n0:185_0": "36.",
  "anya-e0604n0:186_0": "Sati dhammavicayo ca,",
  "anya-e0604n0:187_0": "Tatheva vīriyaṃ pīti;",
  "anya-e0604n0:188_0": "Passaddhi samādhupekkhā,",
  "anya-e0604n0:189_0": "Saddhamma cakkavattikaṃ;",
  "anya-e0604n0:190_0": "Satta bojjhaṅgaratanaṃ,",
  "anya-e0604n0:191_0": "Namāmi purisuttamaṃ.",
  "anya-e0604n0:192_0": "37.",
  "anya-e0604n0:193_0": "Yugandharo īsadharo,",
  "anya-e0604n0:194_0": "Karavīko sudassano;",
  "anya-e0604n0:195_0": "Nemindharo vinatako,",
  "anya-e0604n0:196_0": "Assakaṇṇo mahānage;",
  "anya-e0604n0:197_0": "Sattete hattharūpaṃva,",
  "anya-e0604n0:198_0": "Sampassantaṃ jinaṃ name.",
  "anya-e0604n0:199_0": "38.",
  "anya-e0604n0:200_0": "Kaṇṇamuṇḍo anotatto,",
  "anya-e0604n0:201_0": "Kuṇālo rathakārako;",
  "anya-e0604n0:202_0": "Sīhappapāta chaddantā,",
  "anya-e0604n0:203_0": "Mandākinī mahāsare;",
  "anya-e0604n0:204_0": "Sattete sabbathā passaṃ,",
  "anya-e0604n0:205_0": "Diṭṭheva pātikaṃ name.",
  "anya-e0604n0:206_0": "39.",
  "anya-e0604n0:207_0": "Manussattaṃ liṅgasampatti,",
  "anya-e0604n0:208_0": "Hetu satthāradassanaṃ;",
  "anya-e0604n0:209_0": "Pabbajjā guṇasampatti,",
  "anya-e0604n0:210_0": "Adhikāro ca chandatā;",
  "anya-e0604n0:211_0": "Aṭṭhadhamma samodhānā,",
  "anya-e0604n0:212_0": "Bhinīhārantaguṃ name.",
  "anya-e0604n0:213_0": "40.",
  "anya-e0604n0:214_0": "Visaṭṭhaṃ mañju viññeyyaṃ,",
  "anya-e0604n0:215_0": "Savanīyāvisāriṇā;",
  "anya-e0604n0:216_0": "Gambhīro binduninnādo,",
  "anya-e0604n0:217_0": "Aṭṭhaṅgikasaraṃ name.",
  "anya-e0604n0:218_0": "41.",
  "anya-e0604n0:219_0": "Cattāri rūpajhānāni,",
  "anya-e0604n0:220_0": "Tathevārūpajhānāni;",
  "anya-e0604n0:221_0": "Tā samāpattiyo aṭṭha,",
  "anya-e0604n0:222_0": "Sevantaṃ ruciyā name.",
  "anya-e0604n0:223_0": "42.",
  "anya-e0604n0:224_0": "Aṇimā laṅghimā kammaṃ,",
  "anya-e0604n0:225_0": "Mahimā pattimā tathā;",
  "anya-e0604n0:226_0": "Īsitā vasitā ceva,",
  "anya-e0604n0:227_0": "Yatthakāmāvasāyitā;",
  "anya-e0604n0:228_0": "Aṭṭhissariya puṇṇattā,",
  "anya-e0604n0:229_0": "Issarātissaraṃ name.",
  "anya-e0604n0:230_0": "43.",
  "anya-e0604n0:231_0": "Manomayiddhiñāṇañca,",
  "anya-e0604n0:232_0": "Chaḷabhiññā vipassanā;",
  "anya-e0604n0:233_0": "Aṭṭhavijjāhi sampannaṃ,",
  "anya-e0604n0:234_0": "Vande tilokaketukaṃ.",
  "anya-e0604n0:235_0": "44.",
  "anya-e0604n0:236_0": "Jāti jarā rujā kālo,",
  "anya-e0604n0:237_0": "Caturāpāyadukkhāti;",
  "anya-e0604n0:238_0": "Aṭṭha saṃvegavatthūni,",
  "anya-e0604n0:239_0": "Bhāsantaṃ vividhaṃ name.",
  "anya-e0604n0:240_0": "45.",
  "anya-e0604n0:241_0": "Suttaṅgaṃ veyyākaraṇaṃ,",
  "anya-e0604n0:242_0": "Geyyaṃ gāthā ca jātakaṃ;",
  "anya-e0604n0:243_0": "Abbhūtadhamma vedallā,",
  "anya-e0604n0:244_0": "Udānamiti vuttakaṃ;",
  "anya-e0604n0:245_0": "Navaṅgaṃ sabbalokassa,",
  "anya-e0604n0:246_0": "Dadantaṃ sāsanaṃ name.",
  "anya-e0604n0:247_0": "46.",
  "anya-e0604n0:248_0": "Yā samāpattiyo aṭṭha,",
  "anya-e0604n0:249_0": "Nirodho cāti te nava,",
  "anya-e0604n0:250_0": "Anupubbavihāre taṃ,",
  "anya-e0604n0:251_0": "Sevantaṃ ruciyā name.",
  "anya-e0604n0:252_0": "47.",
  "anya-e0604n0:253_0": "Ekattañceva nānattaṃ,",
  "anya-e0604n0:254_0": "Kāyasaññīhi yogato,",
  "anya-e0604n0:255_0": "Cattāro tathāruppā ca,",
  "anya-e0604n0:256_0": "Asaññīcāti te nava,",
  "anya-e0604n0:257_0": "Sattavāse vibhajitvā,",
  "anya-e0604n0:258_0": "Pakāsentaṃ name jinaṃ.",
  "anya-e0604n0:259_0": "48.",
  "anya-e0604n0:260_0": "Seyyassa sadiso seyyo,",
  "anya-e0604n0:261_0": "Hīnoti tividho vidho,",
  "anya-e0604n0:262_0": "Tathā sadisahīnānaṃ,",
  "anya-e0604n0:263_0": "Navavidhe nudaṃ name.",
  "anya-e0604n0:264_0": "49.",
  "anya-e0604n0:265_0": "Anatthaṃ me carati ca,",
  "anya-e0604n0:266_0": "Tathā cari carissati,",
  "anya-e0604n0:267_0": "Mittassa arinotvatthaṃ,",
  "anya-e0604n0:268_0": "Navāghāte jahaṃ name.",
  "anya-e0604n0:269_0": "50.",
  "anya-e0604n0:270_0": "Dānaṃ sīlañca nikkhamaṃ,",
  "anya-e0604n0:271_0": "Paññā vīriyapañcamaṃ,",
  "anya-e0604n0:272_0": "Khantī saccamadhiṭṭhānaṃ,",
  "anya-e0604n0:273_0": "Mettupekkhāti tādasa,",
  "anya-e0604n0:274_0": "Pūretvā pāramī vande,",
  "anya-e0604n0:275_0": "Sammāsambodhimajjhavaṃ;",
  "anya-e0604n0:276_0": "51.",
  "anya-e0604n0:277_0": "Kāḷāvakañca gaṅgeyyaṃ,",
  "anya-e0604n0:278_0": "Paṇḍaraṃ tamba piṅgalaṃ,",
  "anya-e0604n0:279_0": "Gandha maṅgala hemañca,",
  "anya-e0604n0:280_0": "Uposatha chaddantime.",
  "anya-e0604n0:281_0": "52.",
  "anya-e0604n0:282_0": "Dhāreti dasaposānaṃ,",
  "anya-e0604n0:283_0": "Nāgo kāḷāvako balaṃ;",
  "anya-e0604n0:284_0": "Sahassakoṭiposānaṃ;",
  "anya-e0604n0:285_0": "Chaddantovāranuttamo.",
  "anya-e0604n0:286_0": "53.",
  "anya-e0604n0:287_0": "Dasa sahassakoṭīnaṃ,",
  "anya-e0604n0:288_0": "Posānaṃ baladhāraṇaṃ;",
  "anya-e0604n0:289_0": "Chaddantānaṃ dasannañca,",
  "anya-e0604n0:290_0": "Dasakāyabalaṃ name.",
  "anya-e0604n0:291_0": "54.",
  "anya-e0604n0:292_0": "Ṭhānāṭhāne vipāke ca,",
  "anya-e0604n0:293_0": "Magge sabbatthagāminaṃ;",
  "anya-e0604n0:294_0": "Nānādhātūsu lokesu,",
  "anya-e0604n0:295_0": "Adhimuttimhi pāṇinaṃ;",
  "anya-e0604n0:296_0": "55.",
  "anya-e0604n0:297_0": "Paropariyatte ñāṇaṃ,",
  "anya-e0604n0:298_0": "Indriyānañca pāṇinaṃ;",
  "anya-e0604n0:299_0": "Jhānādīsu ñāṇaṃ pubbe,",
  "anya-e0604n0:300_0": "Nivāse dibbacakkhu ca;",
  "anya-e0604n0:301_0": "Āsavakkhayañāṇanti,",
  "anya-e0604n0:302_0": "Dasabalañāṇaṃ name.",
  "anya-e0604n0:303_0": "56.",
  "anya-e0604n0:304_0": "Mūlagandho sāro pheggu,",
  "anya-e0604n0:305_0": "Taco papaṭiko raso;",
  "anya-e0604n0:306_0": "Pattaṃ pupphaṃ phalañceva,",
  "anya-e0604n0:307_0": "Gandhagandhoti saṃvaraṃ;",
  "anya-e0604n0:308_0": "Sugandhaṃ dasagandhehi,",
  "anya-e0604n0:309_0": "Vilimpantaṃ sadā name.",
  "anya-e0604n0:310_0": "57.",
  "anya-e0604n0:311_0": "Samādhi ñāṇavipphārā,",
  "anya-e0604n0:312_0": "Adhiṭṭhānaṃ vikubbanā;",
  "anya-e0604n0:313_0": "Manomayāriyā iddhi,",
  "anya-e0604n0:314_0": "Tathā kammavipākajā.",
  "anya-e0604n0:315_0": "58.",
  "anya-e0604n0:316_0": "Vijjāmayā puññavato;",
  "anya-e0604n0:317_0": "Payoga paccayiddhiti;",
  "anya-e0604n0:318_0": "Aggapatta dasiddhīnaṃ,",
  "anya-e0604n0:319_0": "Pāṭiheraṃ karaṃ name.",
  "anya-e0604n0:320_0": "59.",
  "anya-e0604n0:321_0": "Muttā maṇi veḷuriyaṃ,",
  "anya-e0604n0:322_0": "Saṅkho sallo pavāḷakaṃ;",
  "anya-e0604n0:323_0": "Suvaṇṇaṃ rajataṃ lohi,",
  "anya-e0604n0:324_0": "Taṅko masāragallanti;",
  "anya-e0604n0:325_0": "Dasadhā ratanehiggaṃ,",
  "anya-e0604n0:326_0": "Bhogiṃ paññāmaṇiṃ name.",
  "anya-e0604n0:327_0": "60.",
  "anya-e0604n0:328_0": "Sithilaṃ dhanitaṃ dīghaṃ,",
  "anya-e0604n0:329_0": "Rassañca garukaṃ lahu;",
  "anya-e0604n0:330_0": "Niggahitaṃ vimuttañca,",
  "anya-e0604n0:331_0": "Sambandhañca vavatthitaṃ;",
  "anya-e0604n0:332_0": "Vivarantaṃ name dhammaṃ,",
  "anya-e0604n0:333_0": "Dasabyañjanabheditaṃ.",
  "anya-e0604n0:334_0": "61.",
  "anya-e0604n0:335_0": "Pasādojo madhuratā,",
  "anya-e0604n0:336_0": "Samatā sukhumālatā;",
  "anya-e0604n0:337_0": "Silesodāratā kanti,",
  "anya-e0604n0:338_0": "Atthabyatti samādhayo;",
  "anya-e0604n0:339_0": "Dasa saddaguṇopetaṃ,",
  "anya-e0604n0:340_0": "Dhammaṃ pātu karaṃ name.",
  "anya-e0604n0:341_0": "62.",
  "anya-e0604n0:342_0": "Lobho doso moho māno,",
  "anya-e0604n0:343_0": "Uddhaccaṃ diṭṭhi saṃsayo;",
  "anya-e0604n0:344_0": "Ahirikamanottappaṃ,",
  "anya-e0604n0:345_0": "Thinanti sabba dāhake;",
  "anya-e0604n0:346_0": "Kilese dasa te saddhiṃ,",
  "anya-e0604n0:347_0": "Vāsanāya jahaṃ name.",
  "anya-e0604n0:348_0": "63.",
  "anya-e0604n0:349_0": "Saterā daṇḍamaṇikā,",
  "anya-e0604n0:350_0": "Macchavilolagaggarā;",
  "anya-e0604n0:351_0": "Sukkhāsani kapisisā,",
  "anya-e0604n0:352_0": "Vicakkā sañña kukkuṭā;",
  "anya-e0604n0:353_0": "Navāsanīhi phālentaṃ,",
  "anya-e0604n0:354_0": "Tibbaṃ paññāsaniṃ name.",
  "anya-e0604n0:355_0": "64.",
  "anya-e0604n0:356_0": "Rodanā kodhāvudho ca,",
  "anya-e0604n0:357_0": "Ujjhantissariyaṃ tathā;",
  "anya-e0604n0:358_0": "Paṭisaṅkhānaṃ,",
  "anya-e0604n0:359_0": "Khanti aṭṭhabalesu taṃ;",
  "anya-e0604n0:360_0": "Pathavīsadisaṃ vande,",
  "anya-e0604n0:361_0": "Aṭṭhamena vināyakaṃ.",
  "anya-e0604n0:362_0": "65.",
  "anya-e0604n0:363_0": "Kāmarogo ca paṭigho,",
  "anya-e0604n0:364_0": "Māno diṭṭhi ca saṃsayo;",
  "anya-e0604n0:365_0": "Bhavarāgo avijjāti,",
  "anya-e0604n0:366_0": "Sattānusayinaṃ name.",
  "anya-e0604n0:367_0": "66.",
  "anya-e0604n0:368_0": "Titthiyāpakatimahāsāvakā,",
  "anya-e0604n0:369_0": "Aggasāvakā paccekabuddhasambuddhā;",
  "anya-e0604n0:370_0": "Visiṭṭhaṃ sabbathā chasu,",
  "anya-e0604n0:371_0": "Pubbe nivāsa viññūsu;",
  "anya-e0604n0:372_0": "Tilokamakuṭaṃ name.",
  "anya-e0604n0:373_0": "67.",
  "anya-e0604n0:374_0": "Buddhacakkhu samantā ca,",
  "anya-e0604n0:375_0": "Ñāṇaṃ yaṃ dibbacakkhu ca;",
  "anya-e0604n0:376_0": "Dhammoti pañca cakkhūhi,",
  "anya-e0604n0:377_0": "Dassāviṃ mārajiṃ name.",
  "anya-e0604n0:378_0": "68.",
  "anya-e0604n0:379_0": "Khuddakā khaṇikā okkantikā ca,",
  "anya-e0604n0:380_0": "Pharaṇā tathā ubbegā;",
  "anya-e0604n0:381_0": "Pañca pītīhi pinentaṃ,",
  "anya-e0604n0:382_0": "Ruciyā ca namāmahaṃ.",
  "anya-e0604n0:383_0": "69.",
  "anya-e0604n0:384_0": "Dukkhaṃ samudayasaccaṃ,",
  "anya-e0604n0:385_0": "Nirodho maggasaccakaṃ;",
  "anya-e0604n0:386_0": "Catusaccamabhijānaṃ,",
  "anya-e0604n0:387_0": "Catukiccehi taṃ name.",
  "anya-e0604n0:388_0": "70.",
  "anya-e0604n0:389_0": "Pīḷanā saṅkhatattho ca,",
  "anya-e0604n0:390_0": "Tapo vipariṇāmanā;",
  "anya-e0604n0:391_0": "Dukkhasaccaṃ catukkehi,",
  "anya-e0604n0:392_0": "Vibhattāviṃ muniṃ name.",
  "anya-e0604n0:393_0": "71.",
  "anya-e0604n0:394_0": "Āyūhanaṃ nidānañca,",
  "anya-e0604n0:395_0": "Saṃyogo palibodhanaṃ;",
  "anya-e0604n0:396_0": "Catukkehi samudayaṃ,",
  "anya-e0604n0:397_0": "Vibhattāviṃ muniṃ name.",
  "anya-e0604n0:398_0": "72.",
  "anya-e0604n0:399_0": "Nissaraṇañca viveko,",
  "anya-e0604n0:400_0": "Asaṅkhatāmataṃ tathā;",
  "anya-e0604n0:401_0": "Nirodhañca catukkehi,",
  "anya-e0604n0:402_0": "Vibhattāviṃ muniṃ name.",
  "anya-e0604n0:403_0": "73.",
  "anya-e0604n0:404_0": "Niyyāniko ca hetvattho,",
  "anya-e0604n0:405_0": "Dassanādhipateyyakaṃ;",
  "anya-e0604n0:406_0": "Maggasaccaṃ catukkehi,",
  "anya-e0604n0:407_0": "Vibhattāviṃ muniṃ name.",
  "anya-e0604n0:408_0": "74.",
  "anya-e0604n0:409_0": "Satthako ca asammoho,",
  "anya-e0604n0:410_0": "Sappāyo ceva gocaro;",
  "anya-e0604n0:411_0": "Catusampajaññā vinā,",
  "anya-e0604n0:412_0": "Bhāviṃ name tathāgataṃ.",
  "anya-e0604n0:413_0": "75.",
  "anya-e0604n0:414_0": "Pahinākhila dukkhāhaṃ,",
  "anya-e0604n0:415_0": "Bhavasāgara pāraguṃ;",
  "anya-e0604n0:416_0": "Vande sāraguṇopetaṃ,",
  "anya-e0604n0:417_0": "Tenamhi bhavapārago.",
  "anya-e0604n0:418_0": "76.",
  "anya-e0604n0:419_0": "Rūpārūpavilāsagga,",
  "anya-e0604n0:420_0": "Rūpācinteyya saṃyuttaṃ;",
  "anya-e0604n0:421_0": "Vande sāraguṇopetaṃ,",
  "anya-e0604n0:422_0": "Tenamhātularūpavā.",
  "anya-e0604n0:423_0": "77.",
  "anya-e0604n0:424_0": "Iddhi iddhi vilāsagga,",
  "anya-e0604n0:425_0": "Iddhi cinteyya saṃyuttaṃ;",
  "anya-e0604n0:426_0": "Vande sāraguṇepetaṃ,",
  "anya-e0604n0:427_0": "Tenamhā tulaiddhimā.",
  "anya-e0604n0:428_0": "78.",
  "anya-e0604n0:429_0": "Vācā vācā vilāsagga,",
  "anya-e0604n0:430_0": "Vācā cinteyya saṃyuttaṃ;",
  "anya-e0604n0:431_0": "Vande sāraguṇopetaṃ,",
  "anya-e0604n0:432_0": "Tenamhā tulavācako.",
  "anya-e0604n0:433_0": "79.",
  "anya-e0604n0:434_0": "Ñāṇa ñāṇa vilāsagga,",
  "anya-e0604n0:435_0": "Ñāṇācinteyya saṃyuttaṃ;",
  "anya-e0604n0:436_0": "Vande sāraguṇopetaṃ,",
  "anya-e0604n0:437_0": "Tenamhā tulañāṇavā.",
  "anya-e0604n0:438_0": "80.",
  "anya-e0604n0:439_0": "Sajjhaṃ hemañca ratanaṃ,",
  "anya-e0604n0:440_0": "Gehaṃ vatthañca bhojanaṃ;",
  "anya-e0604n0:441_0": "Tadaññepi yathācittaṃ,",
  "anya-e0604n0:442_0": "Māpeyyāhaṃ name jinaṃ.",
  "anya-e0604n0:443_0": "81.",
  "anya-e0604n0:444_0": "Sajjhaṃ hemañca ratanaṃ,",
  "anya-e0604n0:445_0": "Gehaṃ vatthañca bhojanaṃ;",
  "anya-e0604n0:446_0": "Tadaññepi yathācittaṃ,",
  "anya-e0604n0:447_0": "Māpeyyaṃ kammajiddhiyā.",
  "anya-e0604n0:448_0": "82.",
  "anya-e0604n0:449_0": "Sattabojjhaṅga ratano,",
  "anya-e0604n0:450_0": "Saddhādiratano muni;",
  "anya-e0604n0:451_0": "Satippabhutiratano,",
  "anya-e0604n0:452_0": "Vande taṃ purisuttamaṃ.",
  "anya-e0604n0:453_0": "83.",
  "anya-e0604n0:454_0": "Cakkādisattaratanaṃ,",
  "anya-e0604n0:455_0": "Muttādiratanaṃ subhaṃ;",
  "anya-e0604n0:456_0": "Vatthappabhutiratanaṃ,",
  "anya-e0604n0:457_0": "Chandakkhaṇe labhāmahaṃ.",
  "anya-e0604n0:458_0": "84.",
  "anya-e0604n0:459_0": "Sampuṇṇacittasaṅkappo,",
  "anya-e0604n0:460_0": "Yathākaṅkhitamāpako,",
  "anya-e0604n0:461_0": "Kenacinabhibhūto yo,",
  "anya-e0604n0:462_0": "Sabbābhibhū name jinaṃ.",
  "anya-e0604n0:463_0": "85.",
  "anya-e0604n0:464_0": "Sampuṇṇacittasaṅkappo,",
  "anya-e0604n0:465_0": "Yathākaṅkhitamāpako,",
  "anya-e0604n0:466_0": "Kenacinabhibhūtamhi,",
  "anya-e0604n0:467_0": "Sabbābhibhū bhave bhave.",
  "anya-e0604n0:468_0": "86.",
  "anya-e0604n0:469_0": "Dasa chaddantarājāva,",
  "anya-e0604n0:470_0": "Dasa kāyabalo muni;",
  "anya-e0604n0:471_0": "Dasa ñāṇabbalotulyo,",
  "anya-e0604n0:472_0": "Vande taṃ samaṇuttamaṃ.",
  "anya-e0604n0:473_0": "87.",
  "anya-e0604n0:474_0": "Atikāyajavo buddho,",
  "anya-e0604n0:475_0": "Raññājavanahaṃsato;",
  "anya-e0604n0:476_0": "Kā kathā ñāṇavegassa,",
  "anya-e0604n0:477_0": "Vande taṃ samaṇuttamaṃ.",
  "anya-e0604n0:478_0": "88.",
  "anya-e0604n0:479_0": "Chaddantanāgarājāva,",
  "anya-e0604n0:480_0": "Bhave bhave mahabbalo;",
  "anya-e0604n0:481_0": "Rājājavanahaṃsova,",
  "anya-e0604n0:482_0": "Paramaggajavo bhave.",
  "anya-e0604n0:483_0": "89.",
  "anya-e0604n0:484_0": "Udakā kāsacārī ca,",
  "anya-e0604n0:485_0": "Mahinimujjako jino;",
  "anya-e0604n0:486_0": "Māpako ca yathā cittaṃ,",
  "anya-e0604n0:487_0": "Vande taṃ ñeyyapāraguṃ.",
  "anya-e0604n0:488_0": "90.",
  "anya-e0604n0:489_0": "Udakā kāsacārī ca,",
  "anya-e0604n0:490_0": "Mahinimujjako bhave;",
  "anya-e0604n0:491_0": "Māpako ca yathācittaṃ,",
  "anya-e0604n0:492_0": "Bhavaso kammajiddhiyā.",
  "anya-e0604n0:493_0": "91.",
  "anya-e0604n0:494_0": "Gāme vane ca sabbattha,",
  "anya-e0604n0:495_0": "Devehi manussehi ca;",
  "anya-e0604n0:496_0": "Ābhatānantalābhassa,",
  "anya-e0604n0:497_0": "Sadā yassa mahesino;",
  "anya-e0604n0:498_0": "Pattalābhaggataṃ lokaṃ,",
  "anya-e0604n0:499_0": "Taṃ vande munipuṅgavaṃ.",
  "anya-e0604n0:500_0": "92.",
  "anya-e0604n0:501_0": "Gāme vane ca sabbattha,",
  "anya-e0604n0:502_0": "Devehi manussehi ca;",
  "anya-e0604n0:503_0": "Ābhatā nantalābho me,",
  "anya-e0604n0:504_0": "Sadā hotu bhavābhave.",
  "anya-e0604n0:505_0": "93.",
  "anya-e0604n0:506_0": "Puññassimassa tejena,",
  "anya-e0604n0:507_0": "Yathā cittaṃ samijjhatu;",
  "anya-e0604n0:508_0": "Sabbicchā sabbacintā ca,",
  "anya-e0604n0:509_0": "Khippaṃ me jātijātiyaṃ.",
  "anya-e0604n0:510_0": "94.",
  "anya-e0604n0:511_0": "Natthīti vacanaṃ dukkhaṃ,",
  "anya-e0604n0:512_0": "Dehīti vacanaṃ tathā;",
  "anya-e0604n0:513_0": "Tasmā natthīti dehīti,",
  "anya-e0604n0:514_0": "Mā me hotu bhavābhave.",
  "anya-e0604n0:515_0": "95.",
  "anya-e0604n0:516_0": "Sabbaṃ paravasaṃ dukkhaṃ,",
  "anya-e0604n0:517_0": "Sabbamissariyaṃ sukhaṃ;",
  "anya-e0604n0:518_0": "Sabbaṃ paravasamatthu,",
  "anya-e0604n0:519_0": "Sabbamissariyaṃ bhave.",
  "anya-e0604n0:520_0": "96.",
  "anya-e0604n0:521_0": "Vitakkento bhajjakāyo,",
  "anya-e0604n0:522_0": "Sabbāvudha vāraṇo ca;",
  "anya-e0604n0:523_0": "Chaddanta vāraṇabalo,",
  "anya-e0604n0:524_0": "Bhaveyyaṃ jātijā tiyaṃ.",
  "anya-e0604n0:525_0": "97.",
  "anya-e0604n0:526_0": "Subhalakkhaṇasampanno,",
  "anya-e0604n0:527_0": "Suvaṇṇavaṇṇavā bhave;",
  "anya-e0604n0:528_0": "Brahmassaro karavika,",
  "anya-e0604n0:529_0": "Bhāṇī ca jātijātiyaṃ.",
  "anya-e0604n0:530_0": "98.",
  "anya-e0604n0:531_0": "Bhūrimapañño sippānaṃ,",
  "anya-e0604n0:532_0": "Sabbesaṃ kusalo bhave;",
  "anya-e0604n0:533_0": "Visajjetuṃ samatthova,",
  "anya-e0604n0:534_0": "Sabbapucchānaṃ ṭhānaso.",
  "anya-e0604n0:535_0": "99.",
  "anya-e0604n0:536_0": "Verādhaṃsīya bhogā ca,",
  "anya-e0604n0:537_0": "Anantākhīṇa bhogavā;",
  "anya-e0604n0:538_0": "Anantābhajja pariso,",
  "anya-e0604n0:539_0": "Bhavaso pāpuṇe sivaṃ.",
  "anya-e0604n0:540_0": "100.",
  "anya-e0604n0:541_0": "Mā nasseyyaṃ pasayheyyaṃ,",
  "anya-e0604n0:542_0": "Ussukkehi pyahaṃ saha;",
  "anya-e0604n0:543_0": "Pañcaverehi koṭīhi,",
  "anya-e0604n0:544_0": "Bhaṇḍaṃ vā akātukāmitaṃ.",
  "anya-e0604n0:545_0": "101.",
  "anya-e0604n0:546_0": "Meghaṃ vātañca ratanaṃ,",
  "anya-e0604n0:547_0": "Dhaññaṃ vatthañca bhojanaṃ;",
  "anya-e0604n0:548_0": "Sabbicchitaṃ tadaññampi,",
  "anya-e0604n0:549_0": "Māpeyyaṃ kammajiddhiyā.",
  "anya-e0604n0:550_0": "102.",
  "anya-e0604n0:551_0": "Puññenetena nibbānaṃ,",
  "anya-e0604n0:552_0": "Santaṃ pappomi tāvatā;",
  "anya-e0604n0:553_0": "Bhaveyyaṃ sabbajātīsu,",
  "anya-e0604n0:554_0": "Catusampattiyā sadā;",
  "anya-e0604n0:555_0": "Catucakkena sampanno,",
  "anya-e0604n0:556_0": "Saddhammehi ca sattahi.",
  "anya-e0604n0:557_0": "103.",
  "anya-e0604n0:558_0": "Sammādiṭṭhi vasupete,",
  "anya-e0604n0:559_0": "Kulamhi seṭṭhasammate,",
  "anya-e0604n0:560_0": "Sabbasakkata saṃsuddhe;",
  "anya-e0604n0:561_0": "Bhave tihetusandhiko.",
  "anya-e0604n0:562_0": "104.",
  "anya-e0604n0:563_0": "Ghāsacchādanaṃ bhogañca,",
  "anya-e0604n0:564_0": "Neva hatthena kātuna,",
  "anya-e0604n0:565_0": "Bhuñjeyyamiddhiyātveva,",
  "anya-e0604n0:566_0": "Māpetvā yāvadatthakaṃ.",
  "anya-e0604n0:567_0": "105.",
  "anya-e0604n0:568_0": "Manussānaṃ atikkamma,",
  "anya-e0604n0:569_0": "Devānaṃ viya bhojanaṃ,",
  "anya-e0604n0:570_0": "Vatthaṃ bhogo ca me hontu,",
  "anya-e0604n0:571_0": "Parivārātisundarā.",
  "anya-e0604n0:572_0": "106.",
  "anya-e0604n0:573_0": "Dasāsuttaṃpi me bhogaṃ,",
  "anya-e0604n0:574_0": "Pañcaverā bhave bhave;",
  "anya-e0604n0:575_0": "Māgaṇheyyuñca nasseyyuṃ,",
  "anya-e0604n0:576_0": "Asoko bhogahetume.",
  "anya-e0604n0:577_0": "107.",
  "anya-e0604n0:578_0": "Sabbaṅgasubha sampanno,",
  "anya-e0604n0:579_0": "Siṅgīnikkha savaṇṇa vā;",
  "anya-e0604n0:580_0": "Bāttiṃsa lakkhaṇūpeto,",
  "anya-e0604n0:581_0": "Atappana rūpa vā bhave.",
  "anya-e0604n0:582_0": "108.",
  "anya-e0604n0:583_0": "Kāyo candanagandho ca,",
  "anya-e0604n0:584_0": "Mukhaṃ uppalagandhikaṃ;",
  "anya-e0604n0:585_0": "Aṭṭhaṅgiko karavika,",
  "anya-e0604n0:586_0": "Mañjūghoso ca me hotu.",
  "anya-e0604n0:587_0": "109.",
  "anya-e0604n0:588_0": "Tikkha gambhīra pañño ca,",
  "anya-e0604n0:589_0": "Hāsātijavapaññavā,",
  "anya-e0604n0:590_0": "Bhūri nibbedha pañño ca;",
  "anya-e0604n0:591_0": "Sabbapañhavisajjano.",
  "anya-e0604n0:592_0": "111.",
  "anya-e0604n0:593_0": "Anto soḷasavassassa,",
  "anya-e0604n0:594_0": "Tipeṭakadharo bhave;",
  "anya-e0604n0:595_0": "Sabbakammesu sippesu,",
  "anya-e0604n0:596_0": "Vijjāṭhānesu pāragū."
}