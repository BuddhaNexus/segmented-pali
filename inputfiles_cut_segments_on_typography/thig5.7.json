{
  "thig5.7:0.1_0": "Therīgāthā 5.7",
  "thig5.7:0.2_0": "Pañcakanipāta",
  "thig5.7:0.3_0": "Sakulātherīgāthā",
  "thig5.7:1.1_0": "“Agārasmiṁ vasantīhaṁ,",
  "thig5.7:1.2_0": "dhammaṁ sutvāna bhikkhuno;",
  "thig5.7:1.3_0": "Addasaṁ virajaṁ dhammaṁ,",
  "thig5.7:1.4_0": "nibbānaṁ padamaccutaṁ.",
  "thig5.7:2.1_0": "Sāhaṁ puttaṁ dhītarañca,",
  "thig5.7:2.2_0": "dhanadhaññañca chaḍḍiya;",
  "thig5.7:2.3_0": "Kese chedāpayitvāna,",
  "thig5.7:2.4_0": "pabbajiṁ anagāriyaṁ.",
  "thig5.7:3.1_0": "Sikkhamānā ahaṁ santī,",
  "thig5.7:3.2_0": "bhāventī maggamañjasaṁ;",
  "thig5.7:3.3_0": "Pahāsiṁ rāgadosañca,",
  "thig5.7:3.4_0": "tadekaṭṭhe ca āsave.",
  "thig5.7:4.1_0": "Bhikkhunī upasampajja,",
  "thig5.7:4.2_0": "pubbajātimanussariṁ;",
  "thig5.7:4.3_0": "Dibbacakkhu visodhitaṁ,",
  "thig5.7:4.4_0": "vimalaṁ sādhubhāvitaṁ.",
  "thig5.7:5.1_0": "Saṅkhāre parato disvā,",
  "thig5.7:5.2_0": "hetujāte palokite;",
  "thig5.7:5.3_0": "Pahāsiṁ āsave sabbe,",
  "thig5.7:5.4_0": "sītibhūtāmhi nibbutā”ti.",
  "thig5.7:6.1_0": "… Sakulā therī …."
}