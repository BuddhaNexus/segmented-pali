{
  "tha-ap119:0.1_0": "Therāpadāna",
  "tha-ap119:0.2_0": "Mahāparivāravagga",
  "tha-ap119:0.3_0": "7. Buddhasaññakattheraapadāna",
  "tha-ap119:1.1_0": "“Yadā vipassī lokaggo,",
  "tha-ap119:1.2_0": "āyusaṅkhāramossaji;",
  "tha-ap119:1.3_0": "Pathavī sampakampittha,",
  "tha-ap119:1.4_0": "medanī jalamekhalā.",
  "tha-ap119:2.1_0": "Otataṁ vitthataṁ mayhaṁ,",
  "tha-ap119:2.2_0": "suvicittavaṭaṁsakaṁ;",
  "tha-ap119:2.3_0": "Bhavanampi pakampittha,",
  "tha-ap119:2.4_0": "buddhassa āyusaṅkhaye.",
  "tha-ap119:3.1_0": "Tāso mayhaṁ samuppanno,",
  "tha-ap119:3.2_0": "bhavane sampakampite;",
  "tha-ap119:3.3_0": "Uppādo nu kimatthāya,",
  "tha-ap119:3.4_0": "āloko vipulo ahu.",
  "tha-ap119:4.1_0": "Vessavaṇo idhāgamma,",
  "tha-ap119:4.2_0": "nibbāpesi mahājanaṁ;",
  "tha-ap119:4.3_0": "Pāṇabhūte bhayaṁ natthi,",
  "tha-ap119:4.4_0": "ekaggā hotha saṁvutā.",
  "tha-ap119:5.1_0": "Aho buddhā aho dhammā,",
  "tha-ap119:5.2_0": "aho no satthu sampadā;",
  "tha-ap119:5.3_0": "Yasmiṁ uppajjamānamhi,",
  "tha-ap119:5.4_0": "pathavī sampakampati.",
  "tha-ap119:6.1_0": "Buddhānubhāvaṁ kittetvā,",
  "tha-ap119:6.2_0": "kappaṁ saggamhi modahaṁ;",
  "tha-ap119:6.3_0": "Avasesesu kappesu,",
  "tha-ap119:6.4_0": "kusalaṁ caritaṁ mayā.",
  "tha-ap119:7.1_0": "Ekanavutito kappe,",
  "tha-ap119:7.2_0": "yaṁ saññamalabhiṁ tadā;",
  "tha-ap119:7.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap119:7.4_0": "buddhasaññāyidaṁ phalaṁ.",
  "tha-ap119:8.1_0": "Ito cuddasakappamhi,",
  "tha-ap119:8.2_0": "rājā āsiṁ patāpavā;",
  "tha-ap119:8.3_0": "Samito nāma nāmena,",
  "tha-ap119:8.4_0": "cakkavattī mahabbalo.",
  "tha-ap119:9.1_0": "Paṭisambhidā catasso,",
  "tha-ap119:9.2_0": "…pe…",
  "tha-ap119:9.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap119:10.1_0": "Itthaṁ sudaṁ āyasmā buddhasaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap119:11.1_0": "Buddhasaññakattherassāpadānaṁ sattamaṁ."
}