{
  "thag2.9:0.1_0": "Theragāthā 2.9",
  "thag2.9:0.2_0": "Dukanipāta",
  "thag2.9:0.3_0": "Paṭhamavagga",
  "thag2.9:0.4_0": "Gotamattheragāthā",
  "thag2.9:1.1_0": "“Sukhaṁ supanti munayo,",
  "thag2.9:1.2_0": "ye itthīsu na bajjhare;",
  "thag2.9:1.3_0": "Sadā ve rakkhitabbāsu,",
  "thag2.9:1.4_0": "yāsu saccaṁ sudullabhaṁ.",
  "thag2.9:2.1_0": "Vadhaṁ carimha te kāma,",
  "thag2.9:2.2_0": "anaṇā dāni te mayaṁ;",
  "thag2.9:2.3_0": "Gacchāma dāni nibbānaṁ,",
  "thag2.9:2.4_0": "yattha gantvā na socatī”ti.",
  "thag2.9:2.5_0": "… Gotamo thero …."
}