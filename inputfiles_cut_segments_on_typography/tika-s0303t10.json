{
  "tika-s0303t10:0_0": "11. Valāhakasaṁyuttavaṇṇanā",
  "tika-s0303t10:1_0": "550- 606.",
  "tika-s0303t10:1_1": "Lokaṁ vālentā saṁvarantā chādentā ahanti pariyesantīti valāhā,",
  "tika-s0303t10:1_2": "devaputtā.",
  "tika-s0303t10:1_3": "Tesaṁ samūho valāhakadevakāyoti āha “valāhakakāyikā”tiādi.",
  "tika-s0303t10:1_4": "Sītakaraṇavalāhakāti sītaharaṇavalāhakā.",
  "tika-s0303t10:1_5": "Sesapadesūti uṇhavalāhakādipadesu.",
  "tika-s0303t10:1_6": "Eseva nayoti “uṇhakaraṇavalāhakā”tiādinā attho veditabbo.",
  "tika-s0303t10:1_7": "Cittaṭṭhapananti “sītaṁ hotū”ti evaṁ cittassa uppādanaṁ.",
  "tika-s0303t10:1_8": "Vassāneti vassakāle.",
  "tika-s0303t10:1_9": "Utusamuṭṭhānamevāti pākatikasītamevāti attho.",
  "tika-s0303t10:1_10": "Uṇhepīti uṇhakāle.",
  "tika-s0303t10:1_11": "Abbhamaṇḍapoti maṇḍapasadisaabbhapaṭalavitānamāha.",
  "tika-s0303t10:1_12": "Abbhaṁ uppajjatīti tahaṁ tahaṁ paṭalaṁ uṭṭhahati.",
  "tika-s0303t10:1_13": "Abbheyevāti abbhakāle eva,",
  "tika-s0303t10:1_14": "vassāneti attho.",
  "tika-s0303t10:1_15": "Atiabbhanti satapaṭalasahassapaṭalaṁ hutvā abbhuṭṭhānaṁ.",
  "tika-s0303t10:1_16": "Cittavesākhamāsesūti vasantakālaṁ sandhāyāha.",
  "tika-s0303t10:1_17": "Tadā hi viddho vigatavalāhako devo bhavituṁ yutto.",
  "tika-s0303t10:1_18": "Uttaradakkhiṇādīti ādi-",
  "tika-s0303t10:1_19": "saddena pacchimavātādiṁ saṅgaṇhāti.",
  "tika-s0303t10:1_20": "Pakativātoti pakatiyā sabhāvena vāyanakavāto.",
  "tika-s0303t10:1_21": "Taṁ utusamuṭṭhānamevāti āhārūpajīvīnaṁ sattānaṁ sādhāraṇakammūpanissayautusamuṭṭhānameva.",
  "tika-s0303t10:1_22": "Esa nayo utusamuṭṭhānasītuṇhavātesupi.",
  "tika-s0303t10:1_23": "Tampi hi āhārūpajīvīnaṁ sattānaṁ sādhāraṇakammūpanissayamevāti.",
  "tika-s0303t10:2_0": "Gītanti meghagītaṁ.",
  "tika-s0303t10:2_1": "Saccakiriyāyāti tādisānaṁ purisavisesānaṁ saccādhiṭṭhānena.",
  "tika-s0303t10:2_2": "Iddhibalenāti iddhimantānaṁ iddhiānubhāvena.",
  "tika-s0303t10:2_3": "Vināsameghenāti kappavināsakameghena.",
  "tika-s0303t10:2_4": "Aññenapi kaṇhapāpikasattānaṁ pāpakammapaccayā uppannavināsameghena vuṭṭhena so so deso vinassateva.",
  "tika-s0303t10:3_0": "Valāhakasaṁyuttavaṇṇanā niṭṭhitā."
}