{
  "thag4.12:0.1_0": "Theragāthā 4.12",
  "thag4.12:0.2_0": "Catukkanipāta",
  "thag4.12:0.3_0": "Paṭhamavagga",
  "thag4.12:0.4_0": "Muditattheragāthā",
  "thag4.12:1.1_0": "“Pabbajiṁ jīvikatthohaṁ,",
  "thag4.12:1.2_0": "laddhāna upasampadaṁ;",
  "thag4.12:1.3_0": "Tato saddhaṁ paṭilabhiṁ,",
  "thag4.12:1.4_0": "daḷhavīriyo parakkamiṁ.",
  "thag4.12:2.1_0": "Kāmaṁ bhijjatuyaṁ kāyo,",
  "thag4.12:2.2_0": "maṁsapesī visīyaruṁ;",
  "thag4.12:2.3_0": "Ubho jaṇṇukasandhīhi,",
  "thag4.12:2.4_0": "jaṅghāyo papatantu me.",
  "thag4.12:3.1_0": "Nāsissaṁ na pivissāmi,",
  "thag4.12:3.2_0": "vihārā ca na nikkhame;",
  "thag4.12:3.3_0": "Napi passaṁ nipātessaṁ,",
  "thag4.12:3.4_0": "taṇhāsalle anūhate.",
  "thag4.12:4.1_0": "Tassa mevaṁ viharato,",
  "thag4.12:4.2_0": "passa vīriyaparakkamaṁ;",
  "thag4.12:4.3_0": "Tisso vijjā anuppattā,",
  "thag4.12:4.4_0": "kataṁ buddhassa sāsanan”ti.",
  "thag4.12:4.5_0": "… Mudito thero …",
  "thag4.12:4.6_0": "Catukkanipāto niṭṭhito.",
  "thag4.12:5.0_0": "Tatruddānaṁ",
  "thag4.12:5.1_0": "Nāgasamālo bhagu ca,",
  "thag4.12:5.2_0": "sabhiyo nandakopi ca;",
  "thag4.12:5.3_0": "Jambuko senako thero,",
  "thag4.12:5.4_0": "sambhūto rāhulopi ca.",
  "thag4.12:6.1_0": "Bhavati candano thero,",
  "thag4.12:6.2_0": "dasete buddhasāvakā;",
  "thag4.12:6.3_0": "Dhammiko sappako thero,",
  "thag4.12:6.4_0": "mudito cāpi te tayo;",
  "thag4.12:6.5_0": "Gāthāyo dve ca paññāsa,",
  "thag4.12:6.6_0": "therā sabbepi terasāti."
}