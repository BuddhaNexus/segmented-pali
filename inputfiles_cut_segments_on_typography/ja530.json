{
  "ja530:0.1_0": "Jātaka",
  "ja530:0.2_0": "Saṭṭhinipāta",
  "ja530:0.3_0": "Soṇakavagga",
  "ja530:0.4_0": "2. Saṅkiccajātaka",
  "ja530:1.1_0": "Disvā nisinnaṁ rājānaṁ,",
  "ja530:1.2_0": "brahmadattaṁ rathesabhaṁ;",
  "ja530:1.3_0": "Athassa paṭivedesi,",
  "ja530:1.4_0": "“yassāsi anukampako.",
  "ja530:2.1_0": "Saṅkiccāyaṁ anuppatto,",
  "ja530:2.2_0": "isīnaṁ sādhusammato;",
  "ja530:2.3_0": "Taramānarūpo niyyāhi,",
  "ja530:2.4_0": "khippaṁ passa mahesinaṁ”.",
  "ja530:3.1_0": "Tato ca rājā taramāno,",
  "ja530:3.2_0": "yuttamāruyha sandanaṁ;",
  "ja530:3.3_0": "Mittāmaccaparibyūḷho,",
  "ja530:3.4_0": "agamāsi rathesabho.",
  "ja530:4.1_0": "Nikkhippa pañca kakudhāni,",
  "ja530:4.2_0": "kāsīnaṁ raṭṭhavaḍḍhano;",
  "ja530:4.3_0": "Vālabījanimuṇhīsaṁ,",
  "ja530:4.4_0": "khaggaṁ chattañcupāhanaṁ.",
  "ja530:5.1_0": "Oruyha rājā yānamhā,",
  "ja530:5.2_0": "ṭhapayitvā paṭicchadaṁ;",
  "ja530:5.3_0": "Āsīnaṁ dāyapassasmiṁ,",
  "ja530:5.4_0": "saṅkiccamupasaṅkami.",
  "ja530:6.1_0": "Upasaṅkamitvā so rājā,",
  "ja530:6.2_0": "sammodi isinā saha;",
  "ja530:6.3_0": "Taṁ kathaṁ vītisāretvā,",
  "ja530:6.4_0": "ekamantaṁ upāvisi.",
  "ja530:7.1_0": "Ekamantaṁ nisinnova,",
  "ja530:7.2_0": "atha kālaṁ amaññatha;",
  "ja530:7.3_0": "Tato pāpāni kammāni,",
  "ja530:7.4_0": "pucchituṁ paṭipajjatha.",
  "ja530:8.1_0": "“Isiṁ pucchāma saṅkiccaṁ,",
  "ja530:8.2_0": "isīnaṁ sādhusammataṁ;",
  "ja530:8.3_0": "Āsīnaṁ dāyapassasmiṁ,",
  "ja530:8.4_0": "isisaṅghapurakkhataṁ.",
  "ja530:9.1_0": "Kaṁ gatiṁ pecca gacchanti,",
  "ja530:9.2_0": "narā dhammāticārino;",
  "ja530:9.3_0": "Aticiṇṇo mayā dhammo,",
  "ja530:9.4_0": "taṁ me akkhāhi pucchito”.",
  "ja530:10.1_0": "Isī avaca saṅkicco,",
  "ja530:10.2_0": "kāsīnaṁ raṭṭhavaḍḍhanaṁ;",
  "ja530:10.3_0": "Āsīnaṁ dāyapassasmiṁ,",
  "ja530:10.4_0": "“mahārāja suṇohi me.",
  "ja530:11.1_0": "Uppathena vajantassa,",
  "ja530:11.2_0": "yo maggamanusāsati;",
  "ja530:11.3_0": "Tassa ce vacanaṁ kayirā,",
  "ja530:11.4_0": "nāssa maggeyya kaṇṭako.",
  "ja530:12.1_0": "Adhammaṁ paṭipannassa,",
  "ja530:12.2_0": "yo dhammamanusāsati;",
  "ja530:12.3_0": "Tassa ce vacanaṁ kayirā,",
  "ja530:12.4_0": "na so gaccheyya duggatiṁ”.",
  "ja530:13.1_0": "“Dhammo patho mahārāja,",
  "ja530:13.2_0": "adhammo pana uppatho;",
  "ja530:13.3_0": "Adhammo nirayaṁ neti,",
  "ja530:13.4_0": "dhammo pāpeti suggatiṁ.",
  "ja530:14.1_0": "Adhammacārino rāja,",
  "ja530:14.2_0": "narā visamajīvino;",
  "ja530:14.3_0": "Yaṁ gatiṁ pecca gacchanti,",
  "ja530:14.4_0": "niraye te suṇohi me.",
  "ja530:15.1_0": "Sañjīvo kāḷasutto ca,",
  "ja530:15.2_0": "saṅghāto dve ca roruvā;",
  "ja530:15.3_0": "Athāparo mahāvīci,",
  "ja530:15.4_0": "tāpano ca patāpano.",
  "ja530:16.1_0": "Iccete aṭṭha nirayā,",
  "ja530:16.2_0": "akkhātā duratikkamā;",
  "ja530:16.3_0": "Ākiṇṇā luddakammehi,",
  "ja530:16.4_0": "paccekā soḷasussadā.",
  "ja530:17.1_0": "Kadariyatāpanā ghorā,",
  "ja530:17.2_0": "accimanto mahabbhayā;",
  "ja530:17.3_0": "Lomahaṁsanarūpā ca,",
  "ja530:17.4_0": "bhesmā paṭibhayā dukhā.",
  "ja530:18.1_0": "Catukkaṇṇā catudvārā,",
  "ja530:18.2_0": "vibhattā bhāgaso mitā;",
  "ja530:18.3_0": "Ayopākārapariyantā,",
  "ja530:18.4_0": "ayasā paṭikujjitā.",
  "ja530:19.1_0": "Tesaṁ ayomayā bhūmi,",
  "ja530:19.2_0": "jalitā tejasā yutā;",
  "ja530:19.3_0": "Samantā yojanasataṁ,",
  "ja530:19.4_0": "phuṭā tiṭṭhanti sabbadā.",
  "ja530:20.1_0": "Ete patanti niraye,",
  "ja530:20.2_0": "uddhampādā avaṁsirā;",
  "ja530:20.3_0": "Isīnaṁ ativattāro,",
  "ja530:20.4_0": "saññatānaṁ tapassinaṁ.",
  "ja530:21.1_0": "Te bhūnahuno paccanti,",
  "ja530:21.2_0": "macchā bilakatā yathā;",
  "ja530:21.3_0": "Saṁvacchare asaṅkheyye,",
  "ja530:21.4_0": "narā kibbisakārino.",
  "ja530:22.1_0": "Ḍayhamānena gattena,",
  "ja530:22.2_0": "niccaṁ santarabāhiraṁ;",
  "ja530:22.3_0": "Nirayā nādhigacchanti,",
  "ja530:22.4_0": "dvāraṁ nikkhamanesino.",
  "ja530:23.1_0": "Puratthimena dhāvanti,",
  "ja530:23.2_0": "tato dhāvanti pacchato;",
  "ja530:23.3_0": "Uttarenapi dhāvanti,",
  "ja530:23.4_0": "tato dhāvanti dakkhiṇaṁ;",
  "ja530:23.5_0": "Yaṁ yañhi dvāraṁ gacchanti,",
  "ja530:23.6_0": "taṁ tadeva pidhīyare.",
  "ja530:24.1_0": "Bahūni vassasahassāni,",
  "ja530:24.2_0": "janā nirayagāmino;",
  "ja530:24.3_0": "Bāhā paggayha kandanti,",
  "ja530:24.4_0": "patvā dukkhaṁ anappakaṁ.",
  "ja530:25.1_0": "Āsīvisaṁva kupitaṁ,",
  "ja530:25.2_0": "tejassiṁ duratikkamaṁ;",
  "ja530:25.3_0": "Na sādhurūpe āsīde,",
  "ja530:25.4_0": "saññatānaṁ tapassinaṁ.",
  "ja530:26.1_0": "Atikāyo mahissāso,",
  "ja530:26.2_0": "ajjuno kekakādhipo;",
  "ja530:26.3_0": "Sahassabāhu ucchinno,",
  "ja530:26.4_0": "isimāsajja gotamaṁ.",
  "ja530:27.1_0": "Arajaṁ rajasā vacchaṁ,",
  "ja530:27.2_0": "Kisaṁ avakiriya daṇḍakī;",
  "ja530:27.3_0": "Tālova mūlato chinno,",
  "ja530:27.4_0": "Sa rājā vibhavaṅgato.",
  "ja530:28.1_0": "Upahacca manaṁ majjho,",
  "ja530:28.2_0": "mātaṅgasmiṁ yasassine;",
  "ja530:28.3_0": "Sapārisajjo ucchinno,",
  "ja530:28.4_0": "majjhāraññaṁ tadā ahu.",
  "ja530:29.1_0": "Kaṇhadīpāyanāsajja,",
  "ja530:29.2_0": "Isiṁ andhakaveṇḍayo;",
  "ja530:29.3_0": "Aññoññaṁ musalā hantvā,",
  "ja530:29.4_0": "Sampattā yamasādhanaṁ.",
  "ja530:30.1_0": "Athāyaṁ isinā satto,",
  "ja530:30.2_0": "antalikkhacaro pure;",
  "ja530:30.3_0": "Pāvekkhi pathaviṁ cecco,",
  "ja530:30.4_0": "hīnatto pattapariyāyaṁ.",
  "ja530:31.1_0": "Tasmā hi chandāgamanaṁ,",
  "ja530:31.2_0": "nappasaṁsanti paṇḍitā;",
  "ja530:31.3_0": "Aduṭṭhacitto bhāseyya,",
  "ja530:31.4_0": "giraṁ saccūpasaṁhitaṁ.",
  "ja530:32.1_0": "Manasā ce paduṭṭhena,",
  "ja530:32.2_0": "yo naro pekkhate muniṁ;",
  "ja530:32.3_0": "Vijjācaraṇasampannaṁ,",
  "ja530:32.4_0": "gantā so nirayaṁ adho.",
  "ja530:33.1_0": "Ye vuḍḍhe paribhāsanti,",
  "ja530:33.2_0": "pharusūpakkamā janā;",
  "ja530:33.3_0": "Anapaccā adāyādā,",
  "ja530:33.4_0": "tālavatthu bhavanti te.",
  "ja530:34.1_0": "Yo ca pabbajitaṁ hanti,",
  "ja530:34.2_0": "katakiccaṁ mahesinaṁ;",
  "ja530:34.3_0": "Sa kāḷasutte niraye,",
  "ja530:34.4_0": "cirarattāya paccati.",
  "ja530:35.1_0": "Yo ca rājā adhammaṭṭho,",
  "ja530:35.2_0": "raṭṭhaviddhaṁsano mago;",
  "ja530:35.3_0": "Tāpayitvā janapadaṁ,",
  "ja530:35.4_0": "tāpane pecca paccati.",
  "ja530:36.1_0": "So ca vassasahassāni,",
  "ja530:36.2_0": "sataṁ dibbāni paccati;",
  "ja530:36.3_0": "Accisaṅghapareto so,",
  "ja530:36.4_0": "dukkhaṁ vedeti vedanaṁ.",
  "ja530:37.1_0": "Tassa aggisikhā kāyā,",
  "ja530:37.2_0": "niccharanti pabhassarā;",
  "ja530:37.3_0": "Tejobhakkhassa gattāni,",
  "ja530:37.4_0": "lomehi ca nakhehi ca.",
  "ja530:38.1_0": "Ḍayhamānena gattena,",
  "ja530:38.2_0": "niccaṁ santarabāhiraṁ;",
  "ja530:38.3_0": "Dukkhābhitunno nadati,",
  "ja530:38.4_0": "nāgo tuttaṭṭito yathā.",
  "ja530:39.1_0": "Yo lobhā pitaraṁ hanti,",
  "ja530:39.2_0": "dosā vā purisādhamo;",
  "ja530:39.3_0": "Sa kāḷasutte niraye,",
  "ja530:39.4_0": "cirarattāya paccati.",
  "ja530:40.1_0": "Sa tādiso paccati lohakumbhiyaṁ,",
  "ja530:40.2_0": "Pakkañca sattīhi hananti nittacaṁ;",
  "ja530:40.3_0": "Andhaṁ karitvā muttakarīsabhakkhaṁ,",
  "ja530:40.4_0": "Khāre nimujjanti tathāvidhaṁ naraṁ.",
  "ja530:41.1_0": "Tattaṁ pakkuthitamayoguḷañca,",
  "ja530:41.2_0": "Dīghe ca phāle cirarattatāpite;",
  "ja530:41.3_0": "Vikkhambhamādāya vibandharajjubhi,",
  "ja530:41.4_0": "Vivaṭe mukhe sampavisanti rakkhasā.",
  "ja530:42.1_0": "Sāmā ca soṇā sabalā ca gijjhā,",
  "ja530:42.2_0": "Kākoḷasaṅghā ca dijā ayomukhā;",
  "ja530:42.3_0": "Saṅgamma khādanti vipphandamānaṁ,",
  "ja530:42.4_0": "Jivhaṁ vibhajja vighāsaṁ salohitaṁ.",
  "ja530:43.1_0": "Taṁ daḍḍhatālaṁ paribhinnagattaṁ,",
  "ja530:43.2_0": "Nippothayantā anuvicaranti rakkhasā;",
  "ja530:43.3_0": "Ratī hi nesaṁ dukhino panītare,",
  "ja530:43.4_0": "Etādisasmiṁ niraye vasanti;",
  "ja530:43.5_0": "Ye keci loke idha pettighātino.",
  "ja530:44.1_0": "Putto ca mātaraṁ hantvā,",
  "ja530:44.2_0": "ito gantvā yamakkhayaṁ;",
  "ja530:44.3_0": "Bhusamāpajjate dukkhaṁ,",
  "ja530:44.4_0": "attakammaphalūpago.",
  "ja530:45.1_0": "Amanussā atibalā,",
  "ja530:45.2_0": "hantāraṁ janayantiyā;",
  "ja530:45.3_0": "Ayomayehi vālehi,",
  "ja530:45.4_0": "pīḷayanti punappunaṁ.",
  "ja530:46.1_0": "Tamassavaṁ sakā gattā,",
  "ja530:46.2_0": "rudhiraṁ attasambhavaṁ;",
  "ja530:46.3_0": "Tambalohavilīnaṁva,",
  "ja530:46.4_0": "tattaṁ pāyenti mattighaṁ.",
  "ja530:47.1_0": "Jigucchaṁ kuṇapaṁ pūtiṁ,",
  "ja530:47.2_0": "Duggandhaṁ gūthakaddamaṁ;",
  "ja530:47.3_0": "Pubbalohitasaṅkāsaṁ,",
  "ja530:47.4_0": "Rahadamogayha tiṭṭhati.",
  "ja530:48.1_0": "Tamenaṁ kimayo tattha,",
  "ja530:48.2_0": "atikāyā ayomukhā;",
  "ja530:48.3_0": "Chaviṁ bhetvāna khādanti,",
  "ja530:48.4_0": "saṅgiddhā maṁsalohite.",
  "ja530:49.1_0": "So ca taṁ nirayaṁ patto,",
  "ja530:49.2_0": "nimuggo sataporisaṁ;",
  "ja530:49.3_0": "Pūtikaṁ kuṇapaṁ vāti,",
  "ja530:49.4_0": "samantā satayojanaṁ.",
  "ja530:50.1_0": "Cakkhumāpi hi cakkhūhi,",
  "ja530:50.2_0": "tena gandhena jīyati;",
  "ja530:50.3_0": "Etādisaṁ brahmadatta,",
  "ja530:50.4_0": "mātugho labhate dukhaṁ.",
  "ja530:51.1_0": "Khuradhāramanukkamma,",
  "ja530:51.2_0": "tikkhaṁ durabhisambhavaṁ;",
  "ja530:51.3_0": "Patanti gabbhapātiyo,",
  "ja530:51.4_0": "duggaṁ vetaraṇiṁ nadiṁ.",
  "ja530:52.1_0": "Ayomayā simbaliyo,",
  "ja530:52.2_0": "soḷasaṅgulakaṇṭakā;",
  "ja530:52.3_0": "Ubhato abhilambanti,",
  "ja530:52.4_0": "duggaṁ vetaraṇiṁ nadiṁ.",
  "ja530:53.1_0": "Te accimanto tiṭṭhanti,",
  "ja530:53.2_0": "aggikkhandhāva ārakā;",
  "ja530:53.3_0": "Ādittā jātavedena,",
  "ja530:53.4_0": "uddhaṁ yojanamuggatā.",
  "ja530:54.1_0": "Ete vajanti niraye,",
  "ja530:54.2_0": "tatte tikhiṇakaṇṭake;",
  "ja530:54.3_0": "Nāriyo ca aticārā,",
  "ja530:54.4_0": "narā ca paradāragū.",
  "ja530:55.1_0": "Te patanti adhokkhandhā,",
  "ja530:55.2_0": "vivattā vihatā puthū;",
  "ja530:55.3_0": "Sayanti vinividdhaṅgā,",
  "ja530:55.4_0": "dīghaṁ jagganti sabbadā.",
  "ja530:56.1_0": "Tato ratyā vivasāne,",
  "ja530:56.2_0": "mahatiṁ pabbatūpamaṁ;",
  "ja530:56.3_0": "Lohakumbhiṁ pavajjanti,",
  "ja530:56.4_0": "tattaṁ aggisamūdakaṁ.",
  "ja530:57.1_0": "Evaṁ divā ca ratto ca,",
  "ja530:57.2_0": "dussīlā mohapārutā;",
  "ja530:57.3_0": "Anubhonti sakaṁ kammaṁ,",
  "ja530:57.4_0": "pubbe dukkaṭamattano.",
  "ja530:58.1_0": "Yā ca bhariyā dhanakkītā,",
  "ja530:58.2_0": "sāmikaṁ atimaññati;",
  "ja530:58.3_0": "Sassuṁ vā sasuraṁ vāpi,",
  "ja530:58.4_0": "jeṭṭhaṁ vāpi nanandaraṁ.",
  "ja530:59.1_0": "Tassā vaṅkena jivhaggaṁ,",
  "ja530:59.2_0": "nibbahanti sabandhanaṁ;",
  "ja530:59.3_0": "Sa byāmamattaṁ kiminaṁ,",
  "ja530:59.4_0": "jivhaṁ passati attani;",
  "ja530:59.5_0": "Viññāpetuṁ na sakkoti,",
  "ja530:59.6_0": "tāpane pecca paccati.",
  "ja530:60.1_0": "Orabbhikā sūkarikā,",
  "ja530:60.2_0": "macchikā migabandhakā;",
  "ja530:60.3_0": "Corā goghātakā luddā,",
  "ja530:60.4_0": "avaṇṇe vaṇṇakārakā.",
  "ja530:61.1_0": "Sattīhi lohakūṭehi,",
  "ja530:61.2_0": "nettiṁsehi usūhi ca;",
  "ja530:61.3_0": "Haññamānā khāranadiṁ,",
  "ja530:61.4_0": "papatanti avaṁsirā.",
  "ja530:62.1_0": "Sāyaṁ pāto kūṭakārī,",
  "ja530:62.2_0": "ayokūṭehi haññati;",
  "ja530:62.3_0": "Tato vantaṁ durattānaṁ,",
  "ja530:62.4_0": "paresaṁ bhuñjare sadā.",
  "ja530:63.1_0": "Dhaṅkā bheraṇḍakā gijjhā,",
  "ja530:63.2_0": "kākoḷā ca ayomukhā;",
  "ja530:63.3_0": "Vipphandamānaṁ khādanti,",
  "ja530:63.4_0": "naraṁ kibbisakārakaṁ.",
  "ja530:64.1_0": "Ye migena migaṁ hanti,",
  "ja530:64.2_0": "pakkhiṁ vā pana pakkhinā;",
  "ja530:64.3_0": "Asanto rajasā channā,",
  "ja530:64.4_0": "gantā te nirayussadaṁ.",
  "ja530:65.1_0": "Santo ca uddhaṁ gacchanti,",
  "ja530:65.2_0": "suciṇṇenidha kammunā;",
  "ja530:65.3_0": "Suciṇṇassa phalaṁ passa,",
  "ja530:65.4_0": "saindā devā sabrahmakā.",
  "ja530:66.1_0": "Taṁ taṁ brūmi mahārāja,",
  "ja530:66.2_0": "Dhammaṁ raṭṭhapatī cara;",
  "ja530:66.3_0": "Tathā rāja carāhi dhammaṁ,",
  "ja530:66.4_0": "Yathā taṁ suciṇṇaṁ nānutappeyya pacchā”ti.",
  "ja530:67.1_0": "Saṅkiccajātakaṁ dutiyaṁ.",
  "ja530:68.1_0": "Saṭṭhinipātaṁ niṭṭhitaṁ.",
  "ja530:69.1_0": "Tassuddānaṁ",
  "ja530:70.1_0": "Atha saṭṭhinipātamhi,",
  "ja530:70.2_0": "Suṇātha mama bhāsitaṁ;",
  "ja530:70.3_0": "Jātakasavhayano pavaro,",
  "ja530:70.4_0": "Soṇakaarindamasavhayano;",
  "ja530:70.5_0": "Tathā vuttarathesabhakiccavaroti."
}