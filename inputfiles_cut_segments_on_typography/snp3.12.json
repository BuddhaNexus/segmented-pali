{
  "snp3.12:0.1_0": "Sutta Nipāta 3.12",
  "snp3.12:0.2_0": "Dvayatānupassanāsutta",
  "snp3.12:1.1_0": "Evaṁ me sutaṁ—",
  "snp3.12:1.2_0": "ekaṁ samayaṁ bhagavā sāvatthiyaṁ viharati pubbārāme migāramātupāsāde.",
  "snp3.12:1.3_0": "Tena kho pana samayena bhagavā tadahuposathe pannarase puṇṇāya puṇṇamāya rattiyā bhikkhusaṅghaparivuto abbhokāse nisinno hoti.",
  "snp3.12:1.4_0": "Atha kho bhagavā tuṇhībhūtaṁ tuṇhībhūtaṁ bhikkhusaṅghaṁ anuviloketvā bhikkhū āmantesi:",
  "snp3.12:2.1_0": "“‘Ye te,",
  "snp3.12:2.1_1": "bhikkhave,",
  "snp3.12:2.1_2": "kusalā dhammā ariyā niyyānikā sambodhagāmino, tesaṁ vo,",
  "snp3.12:2.1_3": "bhikkhave,",
  "snp3.12:2.1_4": "kusalānaṁ dhammānaṁ ariyānaṁ niyyānikānaṁ sambodhagāmīnaṁ kā upanisā savanāyā’ti, iti ce,",
  "snp3.12:2.1_5": "bhikkhave,",
  "snp3.12:2.1_6": "pucchitāro assu,",
  "snp3.12:2.1_7": "te evamassu vacanīyā:",
  "snp3.12:2.2_0": "‘yāvadeva dvayatānaṁ dhammānaṁ yathābhūtaṁ ñāṇāyā’ti.",
  "snp3.12:2.3_0": "Kiñca dvayataṁ vadetha?",
  "snp3.12:3.1_0": "Idaṁ dukkhaṁ, ayaṁ dukkhasamudayoti ayamekānupassanā.",
  "snp3.12:3.2_0": "Ayaṁ dukkhanirodho, ayaṁ dukkhanirodhagāminī paṭipadāti, ayaṁ dutiyānupassanā.",
  "snp3.12:3.3_0": "Evaṁ sammā dvayatānupassino kho,",
  "snp3.12:3.3_1": "bhikkhave,",
  "snp3.12:3.3_2": "bhikkhuno appamattassa ātāpino pahitattassa viharato dvinnaṁ phalānaṁ aññataraṁ phalaṁ pāṭikaṅkhaṁ—",
  "snp3.12:3.4_0": "diṭṭheva dhamme aññā, sati vā upādisese anāgāmitā”ti.",
  "snp3.12:4.1_0": "Idamavoca bhagavā.",
  "snp3.12:4.2_0": "Idaṁ vatvāna sugato athāparaṁ etadavoca satthā:",
  "snp3.12:5.1_0": "“Ye dukkhaṁ nappajānanti,",
  "snp3.12:5.2_0": "atho dukkhassa sambhavaṁ;",
  "snp3.12:5.3_0": "Yattha ca sabbaso dukkhaṁ,",
  "snp3.12:5.4_0": "asesaṁ uparujjhati;",
  "snp3.12:5.5_0": "Tañca maggaṁ na jānanti,",
  "snp3.12:5.6_0": "dukkhūpasamagāminaṁ.",
  "snp3.12:6.1_0": "Cetovimuttihīnā te,",
  "snp3.12:6.2_0": "Atho paññāvimuttiyā;",
  "snp3.12:6.3_0": "Abhabbā te antakiriyāya,",
  "snp3.12:6.4_0": "Te ve jātijarūpagā.",
  "snp3.12:7.1_0": "Ye ca dukkhaṁ pajānanti,",
  "snp3.12:7.2_0": "atho dukkhassa sambhavaṁ;",
  "snp3.12:7.3_0": "Yattha ca sabbaso dukkhaṁ,",
  "snp3.12:7.4_0": "asesaṁ uparujjhati;",
  "snp3.12:7.5_0": "Tañca maggaṁ pajānanti,",
  "snp3.12:7.6_0": "dukkhūpasamagāminaṁ.",
  "snp3.12:8.1_0": "Cetovimuttisampannā,",
  "snp3.12:8.2_0": "Atho paññāvimuttiyā;",
  "snp3.12:8.3_0": "Bhabbā te antakiriyāya,",
  "snp3.12:8.4_0": "Na te jātijarūpagāti.",
  "snp3.12:9.1_0": "‘Siyā aññenapi pariyāyena sammā dvayatānupassanā’ti, iti ce, bhikkhave, pucchitāro assu;",
  "snp3.12:9.2_0": "‘siyā’tissu vacanīyā.",
  "snp3.12:9.3_0": "Kathañca siyā?",
  "snp3.12:9.4_0": "Yaṁ kiñci dukkhaṁ sambhoti sabbaṁ upadhipaccayāti, ayamekānupassanā.",
  "snp3.12:9.5_0": "Upadhīnaṁ tveva asesavirāganirodhā natthi dukkhassa sambhavoti, ayaṁ dutiyānupassanā.",
  "snp3.12:9.6_0": "Evaṁ sammā …pe…",
  "snp3.12:9.7_0": "athāparaṁ etadavoca satthā:",
  "snp3.12:10.1_0": "“Upadhinidānā pabhavanti dukkhā,",
  "snp3.12:10.2_0": "Ye keci lokasmimanekarūpā;",
  "snp3.12:10.3_0": "Yo ve avidvā upadhiṁ karoti,",
  "snp3.12:10.4_0": "Punappunaṁ dukkhamupeti mando;",
  "snp3.12:10.5_0": "Tasmā pajānaṁ upadhiṁ na kayirā,",
  "snp3.12:10.6_0": "Dukkhassa jātippabhavānupassīti.",
  "snp3.12:11.1_0": "‘Siyā aññenapi pariyāyena sammā dvayatānupassanā’ti, iti ce, bhikkhave, pucchitāro assu;",
  "snp3.12:11.2_0": "‘siyā’tissu vacanīyā.",
  "snp3.12:11.3_0": "Kathañca siyā?",
  "snp3.12:11.4_0": "Yaṁ kiñci dukkhaṁ sambhoti sabbaṁ avijjāpaccayāti, ayamekānupassanā.",
  "snp3.12:11.5_0": "Avijjāya tveva asesavirāganirodhā natthi dukkhassa sambhavoti, ayaṁ dutiyānupassanā.",
  "snp3.12:11.6_0": "Evaṁ sammā …pe…",
  "snp3.12:11.7_0": "athāparaṁ etadavoca satthā:",
  "snp3.12:12.1_0": "“Jātimaraṇasaṁsāraṁ,",
  "snp3.12:12.2_0": "ye vajanti punappunaṁ;",
  "snp3.12:12.3_0": "Itthabhāvaññathābhāvaṁ,",
  "snp3.12:12.4_0": "avijjāyeva sā gati.",
  "snp3.12:13.1_0": "Avijjā hāyaṁ mahāmoho,",
  "snp3.12:13.2_0": "Yenidaṁ saṁsitaṁ ciraṁ;",
  "snp3.12:13.3_0": "Vijjāgatā ca ye sattā,",
  "snp3.12:13.4_0": "Na te gacchanti punabbhavanti.",
  "snp3.12:14.1_0": "‘Siyā aññenapi …pe…",
  "snp3.12:14.2_0": "kathañca siyā?",
  "snp3.12:14.3_0": "Yaṁ kiñci dukkhaṁ sambhoti sabbaṁ saṅkhārapaccayāti, ayamekānupassanā.",
  "snp3.12:14.4_0": "Saṅkhārānaṁ tveva asesavirāganirodhā natthi dukkhassa sambhavoti, ayaṁ dutiyānupassanā.",
  "snp3.12:14.5_0": "Evaṁ sammā …pe…",
  "snp3.12:14.6_0": "athāparaṁ etadavoca satthā:",
  "snp3.12:15.1_0": "“Yaṁ kiñci dukkhaṁ sambhoti,",
  "snp3.12:15.2_0": "sabbaṁ saṅkhārapaccayā;",
  "snp3.12:15.3_0": "Saṅkhārānaṁ nirodhena,",
  "snp3.12:15.4_0": "natthi dukkhassa sambhavo.",
  "snp3.12:16.1_0": "Etamādīnavaṁ ñatvā,",
  "snp3.12:16.2_0": "dukkhaṁ saṅkhārapaccayā;",
  "snp3.12:16.3_0": "Sabbasaṅkhārasamathā,",
  "snp3.12:16.4_0": "saññānaṁ uparodhanā;",
  "snp3.12:16.5_0": "Evaṁ dukkhakkhayo hoti,",
  "snp3.12:16.6_0": "etaṁ ñatvā yathātathaṁ.",
  "snp3.12:17.1_0": "Sammaddasā vedaguno,",
  "snp3.12:17.2_0": "Sammadaññāya paṇḍitā;",
  "snp3.12:17.3_0": "Abhibhuyya mārasaṁyogaṁ,",
  "snp3.12:17.4_0": "Na gacchanti punabbhavanti.",
  "snp3.12:18.1_0": "‘Siyā aññenapi …pe…",
  "snp3.12:18.2_0": "kathañca siyā?",
  "snp3.12:18.3_0": "Yaṁ kiñci dukkhaṁ sambhoti sabbaṁ viññāṇapaccayāti, ayamekānupassanā.",
  "snp3.12:18.4_0": "Viññāṇassa tveva asesavirāganirodhā natthi dukkhassa sambhavoti, ayaṁ dutiyānupassanā.",
  "snp3.12:18.5_0": "Evaṁ sammā …pe…",
  "snp3.12:18.6_0": "athāparaṁ etadavoca satthā:",
  "snp3.12:19.1_0": "“Yaṁ kiñci dukkhaṁ sambhoti,",
  "snp3.12:19.2_0": "Sabbaṁ viññāṇapaccayā;",
  "snp3.12:19.3_0": "Viññāṇassa nirodhena,",
  "snp3.12:19.4_0": "Natthi dukkhassa sambhavo.",
  "snp3.12:20.1_0": "Etamādīnavaṁ ñatvā,",
  "snp3.12:20.2_0": "Dukkhaṁ viññāṇapaccayā;",
  "snp3.12:20.3_0": "Viññāṇūpasamā bhikkhu,",
  "snp3.12:20.4_0": "Nicchāto parinibbutoti.",
  "snp3.12:21.1_0": "‘Siyā aññenapi …pe…",
  "snp3.12:21.2_0": "kathañca siyā?",
  "snp3.12:21.3_0": "Yaṁ kiñci dukkhaṁ sambhoti sabbaṁ phassapaccayāti, ayamekānupassanā.",
  "snp3.12:21.4_0": "Phassassa tveva asesavirāganirodhā natthi dukkhassa sambhavoti, ayaṁ dutiyānupassanā.",
  "snp3.12:21.5_0": "Evaṁ sammā …pe…",
  "snp3.12:21.6_0": "athāparaṁ etadavoca satthā:",
  "snp3.12:22.1_0": "“Tesaṁ phassaparetānaṁ,",
  "snp3.12:22.2_0": "Bhavasotānusārinaṁ;",
  "snp3.12:22.3_0": "Kummaggapaṭipannānaṁ,",
  "snp3.12:22.4_0": "Ārā saṁyojanakkhayo.",
  "snp3.12:23.1_0": "Ye ca phassaṁ pariññāya,",
  "snp3.12:23.2_0": "Aññāyupasame ratā;",
  "snp3.12:23.3_0": "Te ve phassābhisamayā,",
  "snp3.12:23.4_0": "Nicchātā parinibbutāti.",
  "snp3.12:24.1_0": "‘Siyā aññenapi …pe…",
  "snp3.12:24.2_0": "kathañca siyā?",
  "snp3.12:24.3_0": "Yaṁ kiñci dukkhaṁ sambhoti sabbaṁ vedanāpaccayāti, ayamekānupassanā.",
  "snp3.12:24.4_0": "Vedanānaṁ tveva asesavirāganirodhā natthi dukkhassa sambhavoti, ayaṁ dutiyānupassanā.",
  "snp3.12:24.5_0": "Evaṁ sammā …pe…",
  "snp3.12:24.6_0": "athāparaṁ etadavoca satthā:",
  "snp3.12:25.1_0": "“Sukhaṁ vā yadi vā dukkhaṁ,",
  "snp3.12:25.2_0": "Adukkhamasukhaṁ saha;",
  "snp3.12:25.3_0": "Ajjhattañca bahiddhā ca,",
  "snp3.12:25.4_0": "Yaṁ kiñci atthi veditaṁ.",
  "snp3.12:26.1_0": "Etaṁ dukkhanti ñatvāna,",
  "snp3.12:26.2_0": "Mosadhammaṁ palokinaṁ;",
  "snp3.12:26.3_0": "Phussa phussa vayaṁ passaṁ,",
  "snp3.12:26.4_0": "Evaṁ tattha vijānati;",
  "snp3.12:26.5_0": "Vedanānaṁ khayā bhikkhu,",
  "snp3.12:26.6_0": "Nicchāto parinibbutoti.",
  "snp3.12:27.1_0": "‘Siyā aññenapi …pe…",
  "snp3.12:27.2_0": "kathañca siyā?",
  "snp3.12:27.3_0": "Yaṁ kiñci dukkhaṁ sambhoti sabbaṁ taṇhāpaccayāti, ayamekānupassanā.",
  "snp3.12:27.4_0": "Taṇhāya tveva asesavirāganirodhā natthi dukkhassa sambhavoti, ayaṁ dutiyānupassanā.",
  "snp3.12:27.5_0": "Evaṁ sammā …pe…",
  "snp3.12:27.6_0": "athāparaṁ etadavoca satthā:",
  "snp3.12:28.1_0": "“Taṇhādutiyo puriso,",
  "snp3.12:28.2_0": "Dīghamaddhāna saṁsaraṁ;",
  "snp3.12:28.3_0": "Itthabhāvaññathābhāvaṁ,",
  "snp3.12:28.4_0": "Saṁsāraṁ nātivattati.",
  "snp3.12:29.1_0": "Etamādīnavaṁ ñatvā,",
  "snp3.12:29.2_0": "Taṇhaṁ dukkhassa sambhavaṁ;",
  "snp3.12:29.3_0": "Vītataṇho anādāno,",
  "snp3.12:29.4_0": "Sato bhikkhu paribbajeti.",
  "snp3.12:30.1_0": "‘Siyā aññenapi …pe…",
  "snp3.12:30.2_0": "kathañca siyā?",
  "snp3.12:30.3_0": "Yaṁ kiñci dukkhaṁ sambhoti sabbaṁ upādānapaccayāti, ayamekānupassanā.",
  "snp3.12:30.4_0": "Upādānānaṁ tveva asesavirāganirodhā natthi dukkhassa sambhavoti, ayaṁ dutiyānupassanā.",
  "snp3.12:30.5_0": "Evaṁ sammā …pe…",
  "snp3.12:30.6_0": "athāparaṁ etadavoca satthā:",
  "snp3.12:31.1_0": "“Upādānapaccayā bhavo,",
  "snp3.12:31.2_0": "Bhūto dukkhaṁ nigacchati;",
  "snp3.12:31.3_0": "Jātassa maraṇaṁ hoti,",
  "snp3.12:31.4_0": "Eso dukkhassa sambhavo.",
  "snp3.12:32.1_0": "Tasmā upādānakkhayā,",
  "snp3.12:32.2_0": "Sammadaññāya paṇḍitā;",
  "snp3.12:32.3_0": "Jātikkhayaṁ abhiññāya,",
  "snp3.12:32.4_0": "Na gacchanti punabbhavanti.",
  "snp3.12:33.1_0": "‘Siyā aññenapi …pe…",
  "snp3.12:33.2_0": "kathañca siyā?",
  "snp3.12:33.3_0": "Yaṁ kiñci dukkhaṁ sambhoti sabbaṁ ārambhapaccayāti, ayamekānupassanā.",
  "snp3.12:33.4_0": "Ārambhānaṁ tveva asesavirāganirodhā natthi dukkhassa sambhavoti, ayaṁ dutiyānupassanā.",
  "snp3.12:33.5_0": "Evaṁ sammā …pe…",
  "snp3.12:33.6_0": "athāparaṁ etadavoca satthā:",
  "snp3.12:34.1_0": "“Yaṁ kiñci dukkhaṁ sambhoti,",
  "snp3.12:34.2_0": "Sabbaṁ ārambhapaccayā;",
  "snp3.12:34.3_0": "Ārambhānaṁ nirodhena,",
  "snp3.12:34.4_0": "Natthi dukkhassa sambhavo.",
  "snp3.12:35.1_0": "Etamādīnavaṁ ñatvā,",
  "snp3.12:35.2_0": "Dukkhaṁ ārambhapaccayā;",
  "snp3.12:35.3_0": "Sabbārambhaṁ paṭinissajja,",
  "snp3.12:35.4_0": "Anārambhe vimuttino.",
  "snp3.12:36.1_0": "Ucchinnabhavataṇhassa,",
  "snp3.12:36.2_0": "Santacittassa bhikkhuno;",
  "snp3.12:36.3_0": "Vikkhīṇo jātisaṁsāro,",
  "snp3.12:36.4_0": "Natthi tassa punabbhavoti.",
  "snp3.12:37.1_0": "‘Siyā aññenapi …pe…",
  "snp3.12:37.2_0": "kathañca siyā?",
  "snp3.12:37.3_0": "Yaṁ kiñci dukkhaṁ sambhoti sabbaṁ āhārapaccayāti, ayamekānupassanā.",
  "snp3.12:37.4_0": "Āhārānaṁ tveva asesavirāganirodhā natthi dukkhassa sambhavoti, ayaṁ dutiyānupassanā.",
  "snp3.12:37.5_0": "Evaṁ sammā …pe…",
  "snp3.12:37.6_0": "athāparaṁ etadavoca satthā:",
  "snp3.12:38.1_0": "“Yaṁ kiñci dukkhaṁ sambhoti,",
  "snp3.12:38.2_0": "Sabbaṁ āhārapaccayā;",
  "snp3.12:38.3_0": "Āhārānaṁ nirodhena,",
  "snp3.12:38.4_0": "Natthi dukkhassa sambhavo.",
  "snp3.12:39.1_0": "Etamādīnavaṁ ñatvā,",
  "snp3.12:39.2_0": "Dukkhaṁ āhārapaccayā;",
  "snp3.12:39.3_0": "Sabbāhāraṁ pariññāya,",
  "snp3.12:39.4_0": "Sabbāhāramanissito.",
  "snp3.12:40.1_0": "Ārogyaṁ sammadaññāya,",
  "snp3.12:40.2_0": "Āsavānaṁ parikkhayā;",
  "snp3.12:40.3_0": "Saṅkhāya sevī dhammaṭṭho,",
  "snp3.12:40.4_0": "Saṅkhyaṁ nopeti vedagūti.",
  "snp3.12:41.1_0": "‘Siyā aññenapi …pe…",
  "snp3.12:41.2_0": "kathañca siyā?",
  "snp3.12:41.3_0": "Yaṁ kiñci dukkhaṁ sambhoti sabbaṁ iñjitapaccayāti, ayamekānupassanā.",
  "snp3.12:41.4_0": "Iñjitānaṁ tveva asesavirāganirodhā natthi dukkhassa sambhavoti, ayaṁ dutiyānupassanā.",
  "snp3.12:41.5_0": "Evaṁ sammā …pe…",
  "snp3.12:41.6_0": "athāparaṁ etadavoca satthā:",
  "snp3.12:42.1_0": "“Yaṁ kiñci dukkhaṁ sambhoti,",
  "snp3.12:42.2_0": "Sabbaṁ iñjitapaccayā;",
  "snp3.12:42.3_0": "Iñjitānaṁ nirodhena,",
  "snp3.12:42.4_0": "Natthi dukkhassa sambhavo.",
  "snp3.12:43.1_0": "Etamādīnavaṁ ñatvā,",
  "snp3.12:43.2_0": "Dukkhaṁ iñjitapaccayā;",
  "snp3.12:43.3_0": "Tasmā hi ejaṁ vossajja,",
  "snp3.12:43.4_0": "Saṅkhāre uparundhiya;",
  "snp3.12:43.5_0": "Anejo anupādāno,",
  "snp3.12:43.6_0": "Sato bhikkhu paribbajeti.",
  "snp3.12:44.1_0": "‘Siyā aññenapi …pe…",
  "snp3.12:44.2_0": "kathañca siyā?",
  "snp3.12:44.3_0": "Nissitassa calitaṁ hotīti, ayamekānupassanā.",
  "snp3.12:44.4_0": "Anissito na calatīti, ayaṁ dutiyānupassanā.",
  "snp3.12:44.5_0": "Evaṁ sammā …pe…",
  "snp3.12:44.6_0": "athāparaṁ etadavoca satthā:",
  "snp3.12:45.1_0": "“Anissito na calati,",
  "snp3.12:45.2_0": "Nissito ca upādiyaṁ;",
  "snp3.12:45.3_0": "Itthabhāvaññathābhāvaṁ,",
  "snp3.12:45.4_0": "Saṁsāraṁ nātivattati.",
  "snp3.12:46.1_0": "Etamādīnavaṁ ñatvā,",
  "snp3.12:46.2_0": "Nissayesu mahabbhayaṁ;",
  "snp3.12:46.3_0": "Anissito anupādāno,",
  "snp3.12:46.4_0": "Sato bhikkhu paribbajeti.",
  "snp3.12:47.1_0": "‘Siyā aññenapi …pe…",
  "snp3.12:47.2_0": "kathañca siyā?",
  "snp3.12:47.3_0": "Rūpehi, bhikkhave, arūpā santatarāti, ayamekānupassanā.",
  "snp3.12:47.4_0": "Arūpehi nirodho santataroti, ayaṁ dutiyānupassanā.",
  "snp3.12:47.5_0": "Evaṁ sammā …pe…",
  "snp3.12:47.6_0": "athāparaṁ etadavoca satthā:",
  "snp3.12:48.1_0": "“Ye ca rūpūpagā sattā,",
  "snp3.12:48.2_0": "Ye ca arūpaṭṭhāyino;",
  "snp3.12:48.3_0": "Nirodhaṁ appajānantā,",
  "snp3.12:48.4_0": "Āgantāro punabbhavaṁ.",
  "snp3.12:49.1_0": "Ye ca rūpe pariññāya,",
  "snp3.12:49.2_0": "Arūpesu asaṇṭhitā;",
  "snp3.12:49.3_0": "Nirodhe ye vimuccanti,",
  "snp3.12:49.4_0": "Te janā maccuhāyinoti.",
  "snp3.12:50.1_0": "‘Siyā aññenapi …pe…",
  "snp3.12:50.2_0": "kathañca siyā?",
  "snp3.12:50.3_0": "Yaṁ,",
  "snp3.12:50.3_1": "bhikkhave,",
  "snp3.12:50.3_2": "sadevakassa lokassa samārakassa sabrahmakassa sassamaṇabrāhmaṇiyā pajāya sadevamanussāya idaṁ saccanti",
  "snp3.12:50.3_3": "upanijjhāyitaṁ tadamariyānaṁ etaṁ musāti yathābhūtaṁ sammappaññāya sudiṭṭhaṁ,",
  "snp3.12:50.3_4": "ayamekānupassanā.",
  "snp3.12:50.4_0": "Yaṁ,",
  "snp3.12:50.4_1": "bhikkhave,",
  "snp3.12:50.4_2": "sadevakassa …pe…",
  "snp3.12:50.4_3": "sadevamanussāya idaṁ musāti upanijjhāyitaṁ,",
  "snp3.12:50.4_4": "tadamariyānaṁ etaṁ saccanti yathābhūtaṁ sammappaññāya sudiṭṭhaṁ,",
  "snp3.12:50.4_5": "ayaṁ dutiyānupassanā.",
  "snp3.12:50.5_0": "Evaṁ sammā …pe…",
  "snp3.12:50.6_0": "athāparaṁ etadavoca satthā:",
  "snp3.12:51.1_0": "“Anattani attamāniṁ,",
  "snp3.12:51.2_0": "Passa lokaṁ sadevakaṁ;",
  "snp3.12:51.3_0": "Niviṭṭhaṁ nāmarūpasmiṁ,",
  "snp3.12:51.4_0": "Idaṁ saccanti maññati.",
  "snp3.12:52.1_0": "Yena yena hi maññanti,",
  "snp3.12:52.2_0": "Tato taṁ hoti aññathā;",
  "snp3.12:52.3_0": "Tañhi tassa musā hoti,",
  "snp3.12:52.4_0": "Mosadhammañhi ittaraṁ.",
  "snp3.12:53.1_0": "Amosadhammaṁ nibbānaṁ,",
  "snp3.12:53.2_0": "Tadariyā saccato vidū;",
  "snp3.12:53.3_0": "Te ve saccābhisamayā,",
  "snp3.12:53.4_0": "Nicchātā parinibbutāti.",
  "snp3.12:54.1_0": "‘Siyā aññenapi pariyāyena sammā dvayatānupassanā’ti, iti ce, bhikkhave, pucchitāro assu;",
  "snp3.12:54.2_0": "‘siyā’tissu vacanīyā.",
  "snp3.12:54.3_0": "Kathañca siyā?",
  "snp3.12:54.4_0": "Yaṁ,",
  "snp3.12:54.4_1": "bhikkhave,",
  "snp3.12:54.4_2": "sadevakassa lokassa samārakassa sabrahmakassa sassamaṇabrāhmaṇiyā pajāya sadevamanussāya idaṁ sukhanti upanijjhāyitaṁ,",
  "snp3.12:54.4_3": "tadamariyānaṁ etaṁ dukkhanti yathābhūtaṁ sammappaññāya sudiṭṭhaṁ,",
  "snp3.12:54.4_4": "ayamekānupassanā.",
  "snp3.12:54.5_0": "Yaṁ,",
  "snp3.12:54.5_1": "bhikkhave,",
  "snp3.12:54.5_2": "sadevakassa …pe…",
  "snp3.12:54.5_3": "sadevamanussāya idaṁ dukkhanti upanijjhāyitaṁ tadamariyānaṁ etaṁ sukhanti yathābhūtaṁ sammappaññāya sudiṭṭhaṁ,",
  "snp3.12:54.5_4": "ayaṁ dutiyānupassanā.",
  "snp3.12:54.6_0": "Evaṁ sammā dvayatānupassino kho,",
  "snp3.12:54.6_1": "bhikkhave,",
  "snp3.12:54.6_2": "bhikkhuno appamattassa ātāpino pahitattassa viharato dvinnaṁ phalānaṁ aññataraṁ phalaṁ pāṭikaṅkhaṁ—",
  "snp3.12:54.7_0": "diṭṭheva dhamme aññā, sati vā upādisese anāgāmitāti.",
  "snp3.12:54.8_0": "Idamavoca bhagavā.",
  "snp3.12:54.9_0": "Idaṁ vatvāna sugato athāparaṁ etadavoca satthā:",
  "snp3.12:55.1_0": "“Rūpā saddā rasā gandhā,",
  "snp3.12:55.2_0": "Phassā dhammā ca kevalā;",
  "snp3.12:55.3_0": "Iṭṭhā kantā manāpā ca,",
  "snp3.12:55.4_0": "Yāvatatthīti vuccati.",
  "snp3.12:56.1_0": "Sadevakassa lokassa,",
  "snp3.12:56.2_0": "Ete vo sukhasammatā;",
  "snp3.12:56.3_0": "Yattha cete nirujjhanti,",
  "snp3.12:56.4_0": "Taṁ nesaṁ dukkhasammataṁ.",
  "snp3.12:57.1_0": "Sukhanti diṭṭhamariyehi,",
  "snp3.12:57.2_0": "Sakkāyassuparodhanaṁ;",
  "snp3.12:57.3_0": "Paccanīkamidaṁ hoti,",
  "snp3.12:57.4_0": "Sabbalokena passataṁ.",
  "snp3.12:58.1_0": "Yaṁ pare sukhato āhu,",
  "snp3.12:58.2_0": "Tadariyā āhu dukkhato;",
  "snp3.12:58.3_0": "Yaṁ pare dukkhato āhu,",
  "snp3.12:58.4_0": "Tadariyā sukhato vidū.",
  "snp3.12:59.1_0": "Passa dhammaṁ durājānaṁ,",
  "snp3.12:59.2_0": "Sampamūḷhetthaviddasu;",
  "snp3.12:59.3_0": "Nivutānaṁ tamo hoti,",
  "snp3.12:59.4_0": "Andhakāro apassataṁ.",
  "snp3.12:60.1_0": "Satañca vivaṭaṁ hoti,",
  "snp3.12:60.2_0": "Āloko passatāmiva;",
  "snp3.12:60.3_0": "Santike na vijānanti,",
  "snp3.12:60.4_0": "Magā dhammassa kovidā.",
  "snp3.12:61.1_0": "Bhavarāgaparetehi,",
  "snp3.12:61.2_0": "Bhavasotānusāribhi;",
  "snp3.12:61.3_0": "Māradheyyānupannehi,",
  "snp3.12:61.4_0": "Nāyaṁ dhammo susambudho.",
  "snp3.12:62.1_0": "Ko nu aññatra mariyehi,",
  "snp3.12:62.2_0": "Padaṁ sambuddhumarahati;",
  "snp3.12:62.3_0": "Yaṁ padaṁ sammadaññāya,",
  "snp3.12:62.4_0": "Parinibbanti anāsavā”ti.",
  "snp3.12:63.1_0": "Idamavoca bhagavā.",
  "snp3.12:63.2_0": "Attamanā te bhikkhū bhagavato bhāsitaṁ abhinandunti.",
  "snp3.12:63.3_0": "Imasmiñca pana veyyākaraṇasmiṁ bhaññamāne saṭṭhimattānaṁ bhikkhūnaṁ anupādāya āsavehi cittāni vimucciṁsūti.",
  "snp3.12:63.4_0": "Dvayatānupassanāsuttaṁ dvādasamaṁ.",
  "snp3.12:64.0_0": "Tassuddānaṁ",
  "snp3.12:64.1_0": "Saccaṁ upadhi avijjā ca,",
  "snp3.12:64.2_0": "saṅkhāre viññāṇapañcamaṁ;",
  "snp3.12:64.3_0": "Phassavedaniyā taṇhā,",
  "snp3.12:64.4_0": "upādānārambhaāhārā;",
  "snp3.12:64.5_0": "Iñjitaṁ calitaṁ rūpaṁ,",
  "snp3.12:64.6_0": "saccaṁ dukkhena soḷasāti.",
  "snp3.12:64.7_0": "Mahāvaggo tatiyo.",
  "snp3.12:65.0_0": "Tassuddānaṁ",
  "snp3.12:65.1_0": "Pabbajjā ca padhānañca,",
  "snp3.12:65.2_0": "subhāsitañca sundari;",
  "snp3.12:65.3_0": "Māghasuttaṁ sabhiyo ca,",
  "snp3.12:65.4_0": "selo sallañca vuccati.",
  "snp3.12:66.1_0": "Vāseṭṭho cāpi kokāli,",
  "snp3.12:66.2_0": "Nālako dvayatānupassanā;",
  "snp3.12:66.3_0": "Dvādasetāni suttāni,",
  "snp3.12:66.4_0": "Mahāvaggoti vuccatīti."
}