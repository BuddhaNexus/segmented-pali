{
  "thag1.37:0.1_0": "Theragāthā 1.37",
  "thag1.37:0.2_0": "Ekakanipāta",
  "thag1.37:0.3_0": "Catutthavagga",
  "thag1.37:0.4_0": "Kumāputtasahāyakattheragāthā",
  "thag1.37:1.1_0": "“Nānājanapadaṁ yanti,",
  "thag1.37:1.2_0": "Vicarantā asaññatā;",
  "thag1.37:1.3_0": "Samādhiñca virādhenti,",
  "thag1.37:1.4_0": "Kiṁsu raṭṭhacariyā karissati;",
  "thag1.37:1.5_0": "Tasmā vineyya sārambhaṁ,",
  "thag1.37:1.6_0": "Jhāyeyya apurakkhato”ti.",
  "thag1.37:1.7_0": "… Kumāputtattherassa sahāyako thero …."
}