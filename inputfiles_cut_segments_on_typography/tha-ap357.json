{
  "tha-ap357:0.1_0": "Therāpadāna",
  "tha-ap357:0.2_0": "Saddasaññakavagga",
  "tha-ap357:0.3_0": "5. Daṇḍadāyakattheraapadāna",
  "tha-ap357:1.1_0": "“Kānanaṁ vanamogayha,",
  "tha-ap357:1.2_0": "veḷuṁ chetvānahaṁ tadā;",
  "tha-ap357:1.3_0": "Ālambanaṁ karitvāna,",
  "tha-ap357:1.4_0": "saṅghassa adadaṁ ahaṁ.",
  "tha-ap357:2.1_0": "Tena cittappasādena,",
  "tha-ap357:2.2_0": "subbate abhivādiya;",
  "tha-ap357:2.3_0": "Ālambanampi datvāna,",
  "tha-ap357:2.4_0": "pakkāmiṁ uttarāmukho.",
  "tha-ap357:3.1_0": "Catunnavutito kappe,",
  "tha-ap357:3.2_0": "yaṁ daṇḍamadadiṁ tadā;",
  "tha-ap357:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap357:3.4_0": "daṇḍadānassidaṁ phalaṁ.",
  "tha-ap357:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap357:4.2_0": "…pe…",
  "tha-ap357:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap357:5.1_0": "Itthaṁ sudaṁ āyasmā daṇḍadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap357:6.1_0": "Daṇḍadāyakattherassāpadānaṁ pañcamaṁ."
}