{
  "ja57:0.1_0": "Jātaka",
  "ja57:0.2_0": "Ekakanipāta",
  "ja57:0.3_0": "Āsīsavagga",
  "ja57:0.4_0": "7. Vānarindajātaka",
  "ja57:1.1_0": "“Yassete caturo dhammā,",
  "ja57:1.2_0": "vānarinda yathā tava;",
  "ja57:1.3_0": "Saccaṁ dhammo dhiti cāgo,",
  "ja57:1.4_0": "diṭṭhaṁ so ativattatī”ti.",
  "ja57:2.1_0": "Vānarindajātakaṁ sattamaṁ."
}