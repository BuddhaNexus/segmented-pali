{
  "patthana16.35:0.1_0": "Paṭṭhānapakaraṇa",
  "patthana16.35:0.2_0": "Dhammānulomapaccanīya (3), Tikadukapaṭṭhānapāḷi (4)",
  "patthana16.35:0.3_0": "16.35. Sekkhattika, Hetuduka",
  "patthana16.35:1.1_0": "Sekkhaṁ hetuṁ dhammaṁ paṭicca nasekkho nahetu dhammo uppajjati hetupaccayā.",
  "patthana16.35:1.1_1": "(Saṅkhittaṁ.) Hetuyā terasa,",
  "patthana16.35:1.1_2": "ārammaṇe nava …pe…",
  "patthana16.35:1.1_3": "āsevane cha …pe…",
  "patthana16.35:1.1_4": "avigate terasa.",
  "patthana16.35:2.1_0": "Sekkhaṁ nahetuṁ dhammaṁ paṭicca naasekkho nanahetu dhammo uppajjati hetupaccayā.",
  "patthana16.35:2.1_1": "(Saṅkhittaṁ.) Hetuyā nava …pe…",
  "patthana16.35:2.1_2": "āsevane cha …pe…",
  "patthana16.35:2.1_3": "avigate nava."
}