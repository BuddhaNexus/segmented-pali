{
  "snp4.10:0.1_0": "Sutta Nipāta 4.10",
  "snp4.10:0.2_0": "Purābhedasutta",
  "snp4.10:1.1_0": "“Kathaṁdassī kathaṁsīlo,",
  "snp4.10:1.2_0": "upasantoti vuccati;",
  "snp4.10:1.3_0": "Taṁ me gotama pabrūhi,",
  "snp4.10:1.4_0": "pucchito uttamaṁ naraṁ”.",
  "snp4.10:2.1_0": "“Vītataṇho purā bhedā,",
  "snp4.10:2.2_0": "(iti bhagavā)",
  "snp4.10:2.3_0": "Pubbamantamanissito;",
  "snp4.10:2.4_0": "Vemajjhe nupasaṅkheyyo,",
  "snp4.10:2.5_0": "Tassa natthi purakkhataṁ.",
  "snp4.10:3.1_0": "Akkodhano asantāsī,",
  "snp4.10:3.2_0": "avikatthī akukkuco;",
  "snp4.10:3.3_0": "Mantabhāṇī anuddhato,",
  "snp4.10:3.4_0": "sa ve vācāyato muni.",
  "snp4.10:4.1_0": "Nirāsatti anāgate,",
  "snp4.10:4.2_0": "atītaṁ nānusocati;",
  "snp4.10:4.3_0": "Vivekadassī phassesu,",
  "snp4.10:4.4_0": "diṭṭhīsu ca na nīyati.",
  "snp4.10:5.1_0": "Patilīno akuhako,",
  "snp4.10:5.2_0": "apihālu amaccharī;",
  "snp4.10:5.3_0": "Appagabbho ajeguccho,",
  "snp4.10:5.4_0": "pesuṇeyye ca no yuto.",
  "snp4.10:6.1_0": "Sātiyesu anassāvī,",
  "snp4.10:6.2_0": "atimāne ca no yuto;",
  "snp4.10:6.3_0": "Saṇho ca paṭibhānavā,",
  "snp4.10:6.4_0": "na saddho na virajjati.",
  "snp4.10:7.1_0": "Lābhakamyā na sikkhati,",
  "snp4.10:7.2_0": "alābhe ca na kuppati;",
  "snp4.10:7.3_0": "Aviruddho ca taṇhāya,",
  "snp4.10:7.4_0": "rasesu nānugijjhati.",
  "snp4.10:8.1_0": "Upekkhako sadā sato,",
  "snp4.10:8.2_0": "na loke maññate samaṁ;",
  "snp4.10:8.3_0": "Na visesī na nīceyyo,",
  "snp4.10:8.4_0": "tassa no santi ussadā.",
  "snp4.10:9.1_0": "Yassa nissayanā natthi,",
  "snp4.10:9.2_0": "ñatvā dhammaṁ anissito;",
  "snp4.10:9.3_0": "Bhavāya vibhavāya vā,",
  "snp4.10:9.4_0": "taṇhā yassa na vijjati.",
  "snp4.10:10.1_0": "Taṁ brūmi upasantoti,",
  "snp4.10:10.2_0": "Kāmesu anapekkhinaṁ;",
  "snp4.10:10.3_0": "Ganthā tassa na vijjanti,",
  "snp4.10:10.4_0": "Atarī so visattikaṁ.",
  "snp4.10:11.1_0": "Na tassa puttā pasavo,",
  "snp4.10:11.2_0": "Khettaṁ vatthuñca vijjati;",
  "snp4.10:11.3_0": "Attā vāpi nirattā vā,",
  "snp4.10:11.4_0": "Na tasmiṁ upalabbhati.",
  "snp4.10:12.1_0": "Yena naṁ vajjuṁ puthujjanā,",
  "snp4.10:12.2_0": "Atho samaṇabrāhmaṇā;",
  "snp4.10:12.3_0": "Taṁ tassa apurakkhataṁ,",
  "snp4.10:12.4_0": "Tasmā vādesu nejati.",
  "snp4.10:13.1_0": "Vītagedho amaccharī,",
  "snp4.10:13.2_0": "Na ussesu vadate muni;",
  "snp4.10:13.3_0": "Na samesu na omesu,",
  "snp4.10:13.4_0": "Kappaṁ neti akappiyo.",
  "snp4.10:14.1_0": "Yassa loke sakaṁ natthi,",
  "snp4.10:14.2_0": "Asatā ca na socati;",
  "snp4.10:14.3_0": "Dhammesu ca na gacchati,",
  "snp4.10:14.4_0": "Sa ve santoti vuccatī”ti.",
  "snp4.10:14.5_0": "Purābhedasuttaṁ dasamaṁ."
}