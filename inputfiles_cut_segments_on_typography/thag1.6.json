{
  "thag1.6:0.1_0": "Theragāthā 1.6",
  "thag1.6:0.2_0": "Ekakanipāta",
  "thag1.6:0.3_0": "Paṭhamavagga",
  "thag1.6:0.4_0": "Sītavaniyattheragāthā",
  "thag1.6:1.1_0": "“Yo sītavanaṁ upagā bhikkhu,",
  "thag1.6:1.2_0": "Eko santusito samāhitatto;",
  "thag1.6:1.3_0": "Vijitāvī apetalomahaṁso,",
  "thag1.6:1.4_0": "Rakkhaṁ kāyagatāsatiṁ dhitimā”ti.",
  "thag1.6:1.5_0": "Itthaṁ sudaṁ āyasmā sītavaniyo thero gāthaṁ abhāsitthāti."
}