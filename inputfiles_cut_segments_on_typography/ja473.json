{
  "ja473:0.1_0": "Jātaka",
  "ja473:0.2_0": "Dvādasakanipāta",
  "ja473:0.3_0": "Cūḷakuṇālavagga",
  "ja473:0.4_0": "10. Mittāmittajātaka",
  "ja473:1.1_0": "“Kāni kammāni kubbānaṁ,",
  "ja473:1.2_0": "kathaṁ viññū parakkame;",
  "ja473:1.3_0": "Amittaṁ jāneyya medhāvī,",
  "ja473:1.4_0": "disvā sutvā ca paṇḍito”.",
  "ja473:2.1_0": "“Na naṁ umhayate disvā,",
  "ja473:2.2_0": "na ca naṁ paṭinandati;",
  "ja473:2.3_0": "Cakkhūni cassa na dadāti,",
  "ja473:2.4_0": "paṭilomañca vattati.",
  "ja473:3.1_0": "Amitte tassa bhajati,",
  "ja473:3.2_0": "mitte tassa na sevati;",
  "ja473:3.3_0": "Vaṇṇakāme nivāreti,",
  "ja473:3.4_0": "akkosante pasaṁsati.",
  "ja473:4.1_0": "Guyhañca tassa nakkhāti,",
  "ja473:4.2_0": "tassa guyhaṁ na gūhati;",
  "ja473:4.3_0": "Kammaṁ tassa na vaṇṇeti,",
  "ja473:4.4_0": "paññassa nappasaṁsati.",
  "ja473:5.1_0": "Abhave nandati tassa,",
  "ja473:5.2_0": "bhave tassa na nandati;",
  "ja473:5.3_0": "Accheraṁ bhojanaṁ laddhā,",
  "ja473:5.4_0": "tassa nuppajjate sati;",
  "ja473:5.5_0": "Tato naṁ nānukampati,",
  "ja473:5.6_0": "aho sopi labheyyito.",
  "ja473:6.1_0": "Iccete soḷasākārā,",
  "ja473:6.2_0": "amittasmiṁ patiṭṭhitā;",
  "ja473:6.3_0": "Yehi amittaṁ jāneyya,",
  "ja473:6.4_0": "disvā sutvā ca paṇḍito”.",
  "ja473:7.1_0": "“Kāni kammāni kubbānaṁ,",
  "ja473:7.2_0": "kathaṁ viññū parakkame;",
  "ja473:7.3_0": "Mittaṁ jāneyya medhāvī,",
  "ja473:7.4_0": "disvā sutvā ca paṇḍito.",
  "ja473:8.1_0": "Pavutthaṁ tassa sarati,",
  "ja473:8.2_0": "āgataṁ abhinandati;",
  "ja473:8.3_0": "Tato kelāyito hoti,",
  "ja473:8.4_0": "vācāya paṭinandati.",
  "ja473:9.1_0": "Mitte tasseva bhajati,",
  "ja473:9.2_0": "amitte tassa na sevati;",
  "ja473:9.3_0": "Akkosante nivāreti,",
  "ja473:9.4_0": "vaṇṇakāme pasaṁsati.",
  "ja473:10.1_0": "Guyhañca tassa akkhāti,",
  "ja473:10.2_0": "tassa guyhañca gūhati;",
  "ja473:10.3_0": "Kammañca tassa vaṇṇeti,",
  "ja473:10.4_0": "paññaṁ tassa pasaṁsati.",
  "ja473:11.1_0": "Bhave ca nandati tassa,",
  "ja473:11.2_0": "abhave tassa na nandati;",
  "ja473:11.3_0": "Accheraṁ bhojanaṁ laddhā,",
  "ja473:11.4_0": "tassa uppajjate sati;",
  "ja473:11.5_0": "Tato naṁ anukampati,",
  "ja473:11.6_0": "aho sopi labheyyito.",
  "ja473:12.1_0": "Iccete soḷasākārā,",
  "ja473:12.2_0": "Mittasmiṁ suppatiṭṭhitā;",
  "ja473:12.3_0": "Yehi mittañca jāneyya,",
  "ja473:12.4_0": "Disvā sutvā ca paṇḍito”ti.",
  "ja473:13.1_0": "Mittāmittajātakaṁ dasamaṁ.",
  "ja473:14.1_0": "Dvādasakanipātaṁ niṭṭhitaṁ.",
  "ja473:15.1_0": "Tassuddānaṁ",
  "ja473:16.1_0": "Lahucitta sasāla kasanti puna,",
  "ja473:16.2_0": "Atha kāma dasakhaluṭṭhānavaro;",
  "ja473:16.3_0": "Atha kaṇha sukosiya meṇḍavaro,",
  "ja473:16.4_0": "Padumo puna mittavarena dasāti."
}