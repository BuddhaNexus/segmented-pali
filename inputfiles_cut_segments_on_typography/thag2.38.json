{
  "thag2.38:0.1_0": "Theragāthā 2.38",
  "thag2.38:0.2_0": "Dukanipāta",
  "thag2.38:0.3_0": "Catutthavagga",
  "thag2.38:0.4_0": "Nisabhattheragāthā",
  "thag2.38:1.1_0": "“Pañca kāmaguṇe hitvā,",
  "thag2.38:1.2_0": "Piyarūpe manorame;",
  "thag2.38:1.3_0": "Saddhāya gharā nikkhamma,",
  "thag2.38:1.4_0": "Dukkhassantakaro bhave.",
  "thag2.38:2.1_0": "Nābhinandāmi maraṇaṁ,",
  "thag2.38:2.2_0": "nābhinandāmi jīvitaṁ;",
  "thag2.38:2.3_0": "Kālañca paṭikaṅkhāmi,",
  "thag2.38:2.4_0": "sampajāno patissato”ti.",
  "thag2.38:2.5_0": "… Nisabho thero …."
}