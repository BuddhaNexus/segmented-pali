{
  "tha-ap84:0.1_0": "Therāpadāna",
  "tha-ap84:0.2_0": "Timiravagga",
  "tha-ap84:0.3_0": "2. Gatasaññakattheraapadāna",
  "tha-ap84:1.1_0": "“Jātiyā sattavassohaṁ,",
  "tha-ap84:1.2_0": "pabbajiṁ anagāriyaṁ;",
  "tha-ap84:1.3_0": "Avandiṁ satthuno pāde,",
  "tha-ap84:1.4_0": "vippasannena cetasā.",
  "tha-ap84:2.1_0": "Sattanaṅgalakīpupphe,",
  "tha-ap84:2.2_0": "ākāse ukkhipiṁ ahaṁ;",
  "tha-ap84:2.3_0": "Tissaṁ buddhaṁ samuddissa,",
  "tha-ap84:2.4_0": "anantaguṇasāgaraṁ.",
  "tha-ap84:3.1_0": "Sugatānugataṁ maggaṁ,",
  "tha-ap84:3.2_0": "pūjetvā haṭṭhamānaso;",
  "tha-ap84:3.3_0": "Añjaliñca tadākāsiṁ,",
  "tha-ap84:3.4_0": "pasanno sehi pāṇibhi.",
  "tha-ap84:4.1_0": "Dvenavute ito kappe,",
  "tha-ap84:4.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap84:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap84:4.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap84:5.1_0": "Ito aṭṭhamake kappe,",
  "tha-ap84:5.2_0": "tayo aggisikhā ahu;",
  "tha-ap84:5.3_0": "Sattaratanasampannā,",
  "tha-ap84:5.4_0": "cakkavattī mahabbalā.",
  "tha-ap84:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap84:6.2_0": "…pe…",
  "tha-ap84:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap84:7.1_0": "Itthaṁ sudaṁ āyasmā gatasaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap84:8.1_0": "Gatasaññakattherassāpadānaṁ dutiyaṁ."
}