{
  "mil7.8.4:0.1_0": "Milindapañha",
  "mil7.8.4:0.2_0": "Opammakathāpañha",
  "mil7.8.4:0.3_0": "Kumbhavagga",
  "mil7.8.4:0.4_0": "4. Khettaṅgapañha",
  "mil7.8.4:1.1_0": "“Bhante nāgasena,",
  "mil7.8.4:1.1_1": "‘khettassa tīṇi aṅgāni gahetabbānī’ti yaṁ vadesi,",
  "mil7.8.4:1.1_2": "katamāni tāni tīṇi aṅgāni gahetabbānī”ti?",
  "mil7.8.4:1.2_0": "“Yathā, mahārāja, khettaṁ mātikāsampannaṁ hoti;",
  "mil7.8.4:1.3_0": "evameva kho, mahārāja, yoginā yogāvacarena sucaritavattappaṭivattamātikāsampannena bhavitabbaṁ.",
  "mil7.8.4:1.4_0": "Idaṁ, mahārāja, khettassa paṭhamaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.8.4:2.1_0": "Puna caparaṁ, mahārāja,",
  "mil7.8.4:2.1_1": "khettaṁ mariyādāsampannaṁ hoti,",
  "mil7.8.4:2.1_2": "tāya ca mariyādāya udakaṁ rakkhitvā dhaññaṁ paripāceti;",
  "mil7.8.4:2.2_0": "evameva kho, mahārāja,",
  "mil7.8.4:2.2_1": "yoginā yogāvacarena sīlahirimariyādāsampannena bhavitabbaṁ,",
  "mil7.8.4:2.2_2": "tāya ca sīlahirimariyādāya sāmaññaṁ rakkhitvā cattāri sāmaññaphalāni gahetabbāni.",
  "mil7.8.4:2.3_0": "Idaṁ, mahārāja, khettassa dutiyaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.8.4:3.1_0": "Puna caparaṁ, mahārāja,",
  "mil7.8.4:3.1_1": "khettaṁ uṭṭhānasampannaṁ hoti,",
  "mil7.8.4:3.1_2": "kassakassa hāsajanakaṁ appampi bījaṁ vuttaṁ bahu hoti,",
  "mil7.8.4:3.1_3": "bahu vuttaṁ bahutaraṁ hoti;",
  "mil7.8.4:3.2_0": "evameva kho, mahārāja,",
  "mil7.8.4:3.2_1": "yoginā yogāvacarena uṭṭhānasampannena vipulaphaladāyinā bhavitabbaṁ,",
  "mil7.8.4:3.2_2": "dāyakānaṁ hāsajanakena bhavitabbaṁ,",
  "mil7.8.4:3.2_3": "yathā appaṁ dinnaṁ bahu hoti,",
  "mil7.8.4:3.2_4": "bahu dinnaṁ bahutaraṁ hoti.",
  "mil7.8.4:3.3_0": "Idaṁ, mahārāja, khettassa tatiyaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.8.4:3.4_0": "Bhāsitampetaṁ, mahārāja, therena upālinā vinayadharena—",
  "mil7.8.4:4.1_0": "‘Khettūpamena bhavitabbaṁ,",
  "mil7.8.4:4.2_0": "Uṭṭhānavipuladāyinā;",
  "mil7.8.4:4.3_0": "Esa khettavaro nāma,",
  "mil7.8.4:4.4_0": "Yo dadāti vipulaṁ phalan’”ti.",
  "mil7.8.4:5.1_0": "Khettaṅgapañho catuttho."
}