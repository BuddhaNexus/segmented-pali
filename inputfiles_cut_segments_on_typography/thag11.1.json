{
  "thag11.1:0.1_0": "Theragāthā 11.1",
  "thag11.1:0.2_0": "Ekādasakanipāta",
  "thag11.1:0.3_0": "Paṭhamavagga",
  "thag11.1:0.4_0": "Saṅkiccattheragāthā",
  "thag11.1:1.1_0": "“Kiṁ tavattho vane tāta,",
  "thag11.1:1.2_0": "ujjuhānova pāvuse;",
  "thag11.1:1.3_0": "Verambhā ramaṇīyā te,",
  "thag11.1:1.4_0": "paviveko hi jhāyinaṁ”.",
  "thag11.1:2.1_0": "“Yathā abbhāni verambho,",
  "thag11.1:2.2_0": "vāto nudati pāvuse;",
  "thag11.1:2.3_0": "Saññā me abhikiranti,",
  "thag11.1:2.4_0": "vivekapaṭisaññutā.",
  "thag11.1:3.1_0": "Apaṇḍaro aṇḍasambhavo,",
  "thag11.1:3.2_0": "Sīvathikāya niketacāriko;",
  "thag11.1:3.3_0": "Uppādayateva me satiṁ,",
  "thag11.1:3.4_0": "Sandehasmiṁ virāganissitaṁ.",
  "thag11.1:4.1_0": "Yañca aññe na rakkhanti,",
  "thag11.1:4.2_0": "yo ca aññe na rakkhati;",
  "thag11.1:4.3_0": "Sa ve bhikkhu sukhaṁ seti,",
  "thag11.1:4.4_0": "kāmesu anapekkhavā.",
  "thag11.1:5.1_0": "Acchodikā puthusilā,",
  "thag11.1:5.2_0": "gonaṅgulamigāyutā;",
  "thag11.1:5.3_0": "Ambusevālasañchannā,",
  "thag11.1:5.4_0": "te selā ramayanti maṁ.",
  "thag11.1:6.1_0": "Vasitaṁ me araññesu,",
  "thag11.1:6.2_0": "kandarāsu guhāsu ca;",
  "thag11.1:6.3_0": "Senāsanesu pantesu,",
  "thag11.1:6.4_0": "vāḷamiganisevite.",
  "thag11.1:7.1_0": "‘Ime haññantu vajjhantu,",
  "thag11.1:7.2_0": "dukkhaṁ pappontu pāṇino’;",
  "thag11.1:7.3_0": "Saṅkappaṁ nābhijānāmi,",
  "thag11.1:7.4_0": "anariyaṁ dosasaṁhitaṁ.",
  "thag11.1:8.1_0": "Pariciṇṇo mayā satthā,",
  "thag11.1:8.2_0": "kataṁ buddhassa sāsanaṁ;",
  "thag11.1:8.3_0": "Ohito garuko bhāro,",
  "thag11.1:8.4_0": "bhavanetti samūhatā.",
  "thag11.1:9.1_0": "Yassa catthāya pabbajito,",
  "thag11.1:9.2_0": "agārasmānagāriyaṁ;",
  "thag11.1:9.3_0": "So me attho anuppatto,",
  "thag11.1:9.4_0": "sabbasaṁyojanakkhayo.",
  "thag11.1:10.1_0": "Nābhinandāmi maraṇaṁ,",
  "thag11.1:10.2_0": "nābhinandāmi jīvitaṁ;",
  "thag11.1:10.3_0": "Kālañca paṭikaṅkhāmi,",
  "thag11.1:10.4_0": "nibbisaṁ bhatako yathā.",
  "thag11.1:11.1_0": "Nābhinandāmi maraṇaṁ,",
  "thag11.1:11.2_0": "nābhinandāmi jīvitaṁ;",
  "thag11.1:11.3_0": "Kālañca paṭikaṅkhāmi,",
  "thag11.1:11.4_0": "sampajāno patissato”ti.",
  "thag11.1:11.5_0": "… Saṅkicco thero …",
  "thag11.1:11.6_0": "Ekādasakanipāto niṭṭhito.",
  "thag11.1:12.0_0": "Tatruddānaṁ",
  "thag11.1:12.1_0": "Saṅkiccathero ekova,",
  "thag11.1:12.2_0": "katakicco anāsavo;",
  "thag11.1:12.3_0": "Ekādasanipātamhi,",
  "thag11.1:12.4_0": "gāthā ekādaseva cāti."
}