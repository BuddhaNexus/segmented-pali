{
  "tha-ap160:0.1_0": "Therāpadāna",
  "tha-ap160:0.2_0": "Bandhujīvakavagga",
  "tha-ap160:0.3_0": "8. Nāgapupphiyattheraapadāna",
  "tha-ap160:1.1_0": "“Suvaccho nāma nāmena,",
  "tha-ap160:1.2_0": "brāhmaṇo mantapāragū;",
  "tha-ap160:1.3_0": "Purakkhato sasissehi,",
  "tha-ap160:1.4_0": "vasate pabbatantare.",
  "tha-ap160:2.1_0": "Padumuttaro nāma jino,",
  "tha-ap160:2.2_0": "āhutīnaṁ paṭiggaho;",
  "tha-ap160:2.3_0": "Mamuddharitukāmo so,",
  "tha-ap160:2.4_0": "āgacchi mama santikaṁ.",
  "tha-ap160:3.1_0": "Vehāsamhi caṅkamati,",
  "tha-ap160:3.2_0": "dhūpāyati jalate tathā;",
  "tha-ap160:3.3_0": "Hāsaṁ mamaṁ viditvāna,",
  "tha-ap160:3.4_0": "pakkāmi pācināmukho.",
  "tha-ap160:4.1_0": "Tañca acchariyaṁ disvā,",
  "tha-ap160:4.2_0": "abbhutaṁ lomahaṁsanaṁ;",
  "tha-ap160:4.3_0": "Nāgapupphaṁ gahetvāna,",
  "tha-ap160:4.4_0": "gatamaggamhi okiriṁ.",
  "tha-ap160:5.1_0": "Satasahassito kappe,",
  "tha-ap160:5.2_0": "yaṁ pupphaṁ okiriṁ ahaṁ;",
  "tha-ap160:5.3_0": "Tena cittappasādena,",
  "tha-ap160:5.4_0": "duggatiṁ nupapajjahaṁ.",
  "tha-ap160:6.1_0": "Ekattiṁse kappasate,",
  "tha-ap160:6.2_0": "rājā āsi mahāraho;",
  "tha-ap160:6.3_0": "Sattaratanasampanno,",
  "tha-ap160:6.4_0": "cakkavattī mahabbalo.",
  "tha-ap160:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap160:7.2_0": "…pe…",
  "tha-ap160:7.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap160:8.1_0": "Itthaṁ sudaṁ āyasmā nāgapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap160:9.1_0": "Nāgapupphiyattherassāpadānaṁ aṭṭhamaṁ."
}