{
  "tha-ap543:0.1_0": "Therāpadāna",
  "tha-ap543:0.2_0": "Bhaddiyavagga",
  "tha-ap543:0.3_0": "1 Lakuṇḍabhaddiyattheraapadāna",
  "tha-ap543:1.1_0": "“Padumuttaro nāma jino,",
  "tha-ap543:1.2_0": "sabbadhammesu cakkhumā;",
  "tha-ap543:1.3_0": "Ito satasahassamhi,",
  "tha-ap543:1.4_0": "kappe uppajji nāyako.",
  "tha-ap543:2.1_0": "Tadāhaṁ haṁsavatiyaṁ,",
  "tha-ap543:2.2_0": "seṭṭhiputto mahaddhano;",
  "tha-ap543:2.3_0": "Jaṅghāvihāraṁ vicaraṁ,",
  "tha-ap543:2.4_0": "saṅghārāmaṁ agacchahaṁ.",
  "tha-ap543:3.1_0": "Tadā so lokapajjoto,",
  "tha-ap543:3.2_0": "dhammaṁ desesi nāyako;",
  "tha-ap543:3.3_0": "Mañjussarānaṁ pavaraṁ,",
  "tha-ap543:3.4_0": "sāvakaṁ abhikittayi.",
  "tha-ap543:4.1_0": "Taṁ sutvā mudito hutvā,",
  "tha-ap543:4.2_0": "kāraṁ katvā mahesino;",
  "tha-ap543:4.3_0": "Vanditvā satthuno pāde,",
  "tha-ap543:4.4_0": "taṁ ṭhānamabhipatthayiṁ.",
  "tha-ap543:5.1_0": "Tadā buddho viyākāsi,",
  "tha-ap543:5.2_0": "saṅghamajjhe vināyako;",
  "tha-ap543:5.3_0": "‘Anāgatamhi addhāne,",
  "tha-ap543:5.4_0": "lacchase taṁ manorathaṁ.",
  "tha-ap543:6.1_0": "Satasahassito kappe,",
  "tha-ap543:6.2_0": "okkākakulasambhavo;",
  "tha-ap543:6.3_0": "Gotamo nāma gottena,",
  "tha-ap543:6.4_0": "satthā loke bhavissati.",
  "tha-ap543:7.1_0": "Tassa dhammesu dāyādo,",
  "tha-ap543:7.2_0": "oraso dhammanimmito;",
  "tha-ap543:7.3_0": "Bhaddiyo nāma nāmena,",
  "tha-ap543:7.4_0": "hessati satthu sāvako’.",
  "tha-ap543:8.1_0": "Tena kammena sukatena,",
  "tha-ap543:8.2_0": "cetanāpaṇidhīhi ca;",
  "tha-ap543:8.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "tha-ap543:8.4_0": "tāvatiṁsamagacchahaṁ.",
  "tha-ap543:9.1_0": "Dvenavute ito kappe,",
  "tha-ap543:9.2_0": "phusso uppajji nāyako;",
  "tha-ap543:9.3_0": "Durāsado duppasaho,",
  "tha-ap543:9.4_0": "sabbalokuttamo jino.",
  "tha-ap543:10.1_0": "Caraṇena ca sampanno,",
  "tha-ap543:10.2_0": "brahā uju patāpavā;",
  "tha-ap543:10.3_0": "Hitesī sabbasattānaṁ,",
  "tha-ap543:10.4_0": "bahuṁ mocesi bandhanā.",
  "tha-ap543:11.1_0": "Nandārāmavane tassa,",
  "tha-ap543:11.2_0": "ahosiṁ phussakokilo;",
  "tha-ap543:11.3_0": "Gandhakuṭisamāsanne,",
  "tha-ap543:11.4_0": "ambarukkhe vasāmahaṁ.",
  "tha-ap543:12.1_0": "Tadā piṇḍāya gacchantaṁ,",
  "tha-ap543:12.2_0": "dakkhiṇeyyaṁ jinuttamaṁ;",
  "tha-ap543:12.3_0": "Disvā cittaṁ pasādetvā,",
  "tha-ap543:12.4_0": "mañjunābhinikūjahaṁ.",
  "tha-ap543:13.1_0": "Rājuyyānaṁ tadā gantvā,",
  "tha-ap543:13.2_0": "supakkaṁ kanakattacaṁ;",
  "tha-ap543:13.3_0": "Ambapiṇḍaṁ gahetvāna,",
  "tha-ap543:13.4_0": "sambuddhassopanāmayiṁ.",
  "tha-ap543:14.1_0": "Tadā me cittamaññāya,",
  "tha-ap543:14.2_0": "mahākāruṇiko jino;",
  "tha-ap543:14.3_0": "Upaṭṭhākassa hatthato,",
  "tha-ap543:14.4_0": "pattaṁ paggaṇhi nāyako.",
  "tha-ap543:15.1_0": "Adāsiṁ haṭṭhacittohaṁ,",
  "tha-ap543:15.2_0": "ambapiṇḍaṁ mahāmune;",
  "tha-ap543:15.3_0": "Patte pakkhippa pakkhehi,",
  "tha-ap543:15.4_0": "pañjaliṁ katvāna mañjunā.",
  "tha-ap543:16.1_0": "Sarena rajanīyena,",
  "tha-ap543:16.2_0": "savanīyena vaggunā;",
  "tha-ap543:16.3_0": "Vassanto buddhapūjatthaṁ,",
  "tha-ap543:16.4_0": "nīḷaṁ gantvā nipajjahaṁ.",
  "tha-ap543:17.1_0": "Tadā muditacittaṁ maṁ,",
  "tha-ap543:17.2_0": "buddhapemagatāsayaṁ;",
  "tha-ap543:17.3_0": "Sakuṇagghi upāgantvā,",
  "tha-ap543:17.4_0": "ghātayī duṭṭhamānaso.",
  "tha-ap543:18.1_0": "Tato cutohaṁ tusite,",
  "tha-ap543:18.2_0": "anubhotvā mahāsukhaṁ;",
  "tha-ap543:18.3_0": "Manussayonimāgacchiṁ,",
  "tha-ap543:18.4_0": "tassa kammassa vāhasā.",
  "tha-ap543:19.1_0": "Imamhi bhaddake kappe,",
  "tha-ap543:19.2_0": "brahmabandhu mahāyaso;",
  "tha-ap543:19.3_0": "Kassapo nāma gottena,",
  "tha-ap543:19.4_0": "uppajji vadataṁ varo.",
  "tha-ap543:20.1_0": "Sāsanaṁ jotayitvā so,",
  "tha-ap543:20.2_0": "abhibhuyya kutitthiye;",
  "tha-ap543:20.3_0": "Vinayitvāna veneyye,",
  "tha-ap543:20.4_0": "nibbuto so sasāvako.",
  "tha-ap543:21.1_0": "Nibbute tamhi lokagge,",
  "tha-ap543:21.2_0": "pasannā janatā bahū;",
  "tha-ap543:21.3_0": "Pūjanatthāya buddhassa,",
  "tha-ap543:21.4_0": "thūpaṁ kubbanti satthuno.",
  "tha-ap543:22.1_0": "‘Sattayojanikaṁ thūpaṁ,",
  "tha-ap543:22.2_0": "sattaratanabhūsitaṁ;",
  "tha-ap543:22.3_0": "Karissāma mahesissa’,",
  "tha-ap543:22.4_0": "iccevaṁ mantayanti te.",
  "tha-ap543:23.1_0": "Kikino kāsirājassa,",
  "tha-ap543:23.2_0": "tadā senāya nāyako;",
  "tha-ap543:23.3_0": "Hutvāhaṁ appamāṇassa,",
  "tha-ap543:23.4_0": "pamāṇaṁ cetiye vadiṁ.",
  "tha-ap543:24.1_0": "Tadā te mama vākyena,",
  "tha-ap543:24.2_0": "cetiyaṁ yojanuggataṁ;",
  "tha-ap543:24.3_0": "Akaṁsu naravīrassa,",
  "tha-ap543:24.4_0": "nānāratanabhūsitaṁ.",
  "tha-ap543:25.1_0": "Tena kammena sukatena,",
  "tha-ap543:25.2_0": "cetanāpaṇidhīhi ca;",
  "tha-ap543:25.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "tha-ap543:25.4_0": "tāvatiṁsamagacchahaṁ.",
  "tha-ap543:26.1_0": "Pacchime ca bhave dāni,",
  "tha-ap543:26.2_0": "jāto seṭṭhikule ahaṁ;",
  "tha-ap543:26.3_0": "Sāvatthiyaṁ puravare,",
  "tha-ap543:26.4_0": "iddhe phīte mahaddhane.",
  "tha-ap543:27.1_0": "Purappavese sugataṁ,",
  "tha-ap543:27.2_0": "disvā vimhitamānaso;",
  "tha-ap543:27.3_0": "Pabbajitvāna na ciraṁ,",
  "tha-ap543:27.4_0": "arahattamapāpuṇiṁ.",
  "tha-ap543:28.1_0": "Cetiyassa pamāṇaṁ yaṁ,",
  "tha-ap543:28.2_0": "akariṁ tena kammunā;",
  "tha-ap543:28.3_0": "Lakuṇḍakasarīrohaṁ,",
  "tha-ap543:28.4_0": "jāto paribhavāraho.",
  "tha-ap543:29.1_0": "Sarena madhurenāhaṁ,",
  "tha-ap543:29.2_0": "pūjitvā isisattamaṁ;",
  "tha-ap543:29.3_0": "Mañjussarānaṁ bhikkhūnaṁ,",
  "tha-ap543:29.4_0": "aggattamanupāpuṇiṁ.",
  "tha-ap543:30.1_0": "Phaladānena buddhassa,",
  "tha-ap543:30.2_0": "guṇānussaraṇena ca;",
  "tha-ap543:30.3_0": "Sāmaññaphalasampanno,",
  "tha-ap543:30.4_0": "viharāmi anāsavo.",
  "tha-ap543:31.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap543:31.2_0": "bhavā sabbe samūhatā;",
  "tha-ap543:31.3_0": "Nāgova bandhanaṁ chetvā,",
  "tha-ap543:31.4_0": "viharāmi anāsavo.",
  "tha-ap543:32.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap543:32.2_0": "buddhaseṭṭhassa santike;",
  "tha-ap543:32.3_0": "Tisso vijjā anuppattā,",
  "tha-ap543:32.4_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap543:33.1_0": "Paṭisambhidā catasso,",
  "tha-ap543:33.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap543:33.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap543:33.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap543:34.1_0": "Itthaṁ sudaṁ āyasmā lakuṇḍabhaddiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap543:35.1_0": "Lakuṇḍabhaddiyattherassāpadānaṁ paṭhamaṁ."
}