{
  "ja201:0.1_0": "Jātaka",
  "ja201:0.2_0": "Dukanipāta",
  "ja201:0.3_0": "Nataṁdaḷhavagga",
  "ja201:0.4_0": "1. Bandhanāgārajātaka",
  "ja201:1.1_0": "“Na taṁ daḷhaṁ bandhanamāhu dhīrā,",
  "ja201:1.2_0": "Yadāyasaṁ dārujapabbajañca;",
  "ja201:1.3_0": "Sārattarattā maṇikuṇḍalesu,",
  "ja201:1.4_0": "Puttesu dāresu ca yā apekkhā.",
  "ja201:2.1_0": "Etaṁ daḷhaṁ bandhanamāhu dhīrā,",
  "ja201:2.2_0": "Ohārinaṁ sīthilaṁ duppamuñcaṁ;",
  "ja201:2.3_0": "Etampi chetvāna vajanti dhīrā,",
  "ja201:2.4_0": "Anapekkhino kāmasukhaṁ pahāyā”ti.",
  "ja201:3.1_0": "Bandhanāgārajātakaṁ paṭhamaṁ."
}