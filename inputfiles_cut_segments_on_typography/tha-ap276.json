{
  "tha-ap276:0.1_0": "Therāpadāna",
  "tha-ap276:0.2_0": "Suvaṇṇabibbohanavagga",
  "tha-ap276:0.3_0": "4. Abbhañjanadāyakattheraapadāna",
  "tha-ap276:1.1_0": "“Koṇḍaññassa bhagavato,",
  "tha-ap276:1.2_0": "vītarāgassa tādino;",
  "tha-ap276:1.3_0": "Ākāsasamacittassa,",
  "tha-ap276:1.4_0": "nippapañcassa jhāyino.",
  "tha-ap276:2.1_0": "Sabbamohātivattassa,",
  "tha-ap276:2.2_0": "sabbalokahitesino;",
  "tha-ap276:2.3_0": "Abbhañjanaṁ mayā dinnaṁ,",
  "tha-ap276:2.4_0": "dvipadindassa tādino.",
  "tha-ap276:3.1_0": "Aparimeyye ito kappe,",
  "tha-ap276:3.2_0": "abbhañjanamadaṁ tadā;",
  "tha-ap276:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap276:3.4_0": "abbhañjanassidaṁ phalaṁ.",
  "tha-ap276:4.1_0": "Ito pannarase kappe,",
  "tha-ap276:4.2_0": "cirappo nāma khattiyo;",
  "tha-ap276:4.3_0": "Sattaratanasampanno,",
  "tha-ap276:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap276:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap276:5.2_0": "…pe…",
  "tha-ap276:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap276:6.1_0": "Itthaṁ sudaṁ āyasmā abbhañjanadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap276:7.1_0": "Abbhañjanadāyakattherassāpadānaṁ catutthaṁ."
}