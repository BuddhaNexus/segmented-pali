{
  "snp2.1:0.1_0": "Sutta Nipāta 2.1",
  "snp2.1:0.2_0": "Ratanasutta",
  "snp2.1:1.1_0": "Yānīdha bhūtāni samāgatāni,",
  "snp2.1:1.2_0": "Bhummāni vā yāni va antalikkhe;",
  "snp2.1:1.3_0": "Sabbeva bhūtā sumanā bhavantu,",
  "snp2.1:1.4_0": "Athopi sakkacca suṇantu bhāsitaṁ.",
  "snp2.1:2.1_0": "Tasmā hi bhūtā nisāmetha sabbe,",
  "snp2.1:2.2_0": "Mettaṁ karotha mānusiyā pajāya;",
  "snp2.1:2.3_0": "Divā ca ratto ca haranti ye baliṁ,",
  "snp2.1:2.4_0": "Tasmā hi ne rakkhatha appamattā.",
  "snp2.1:3.1_0": "Yaṁ kiñci vittaṁ idha vā huraṁ vā,",
  "snp2.1:3.2_0": "Saggesu vā yaṁ ratanaṁ paṇītaṁ;",
  "snp2.1:3.3_0": "Na no samaṁ atthi tathāgatena,",
  "snp2.1:3.4_0": "Idampi buddhe ratanaṁ paṇītaṁ;",
  "snp2.1:3.5_0": "Etena saccena suvatthi hotu.",
  "snp2.1:4.1_0": "Khayaṁ virāgaṁ amataṁ paṇītaṁ,",
  "snp2.1:4.2_0": "Yadajjhagā sakyamunī samāhito;",
  "snp2.1:4.3_0": "Na tena dhammena samatthi kiñci,",
  "snp2.1:4.4_0": "Idampi dhamme ratanaṁ paṇītaṁ;",
  "snp2.1:4.5_0": "Etena saccena suvatthi hotu.",
  "snp2.1:5.1_0": "Yaṁ buddhaseṭṭho parivaṇṇayī suciṁ,",
  "snp2.1:5.2_0": "Samādhimānantarikaññamāhu;",
  "snp2.1:5.3_0": "Samādhinā tena samo na vijjati,",
  "snp2.1:5.4_0": "Idampi dhamme ratanaṁ paṇītaṁ;",
  "snp2.1:5.5_0": "Etena saccena suvatthi hotu.",
  "snp2.1:6.1_0": "Ye puggalā aṭṭha sataṁ pasatthā,",
  "snp2.1:6.2_0": "Cattāri etāni yugāni honti;",
  "snp2.1:6.3_0": "Te dakkhiṇeyyā sugatassa sāvakā,",
  "snp2.1:6.4_0": "Etesu dinnāni mahapphalāni;",
  "snp2.1:6.5_0": "Idampi saṅghe ratanaṁ paṇītaṁ,",
  "snp2.1:6.6_0": "Etena saccena suvatthi hotu.",
  "snp2.1:7.1_0": "Ye suppayuttā manasā daḷhena,",
  "snp2.1:7.2_0": "Nikkāmino gotamasāsanamhi;",
  "snp2.1:7.3_0": "Te pattipattā amataṁ vigayha,",
  "snp2.1:7.4_0": "Laddhā mudhā nibbutiṁ bhuñjamānā;",
  "snp2.1:7.5_0": "Idampi saṅghe ratanaṁ paṇītaṁ,",
  "snp2.1:7.6_0": "Etena saccena suvatthi hotu.",
  "snp2.1:8.1_0": "Yathindakhīlo pathavissito siyā,",
  "snp2.1:8.2_0": "Catubbhi vātehi asampakampiyo;",
  "snp2.1:8.3_0": "Tathūpamaṁ sappurisaṁ vadāmi,",
  "snp2.1:8.4_0": "Yo ariyasaccāni avecca passati;",
  "snp2.1:8.5_0": "Idampi saṅghe ratanaṁ paṇītaṁ,",
  "snp2.1:8.6_0": "Etena saccena suvatthi hotu.",
  "snp2.1:9.1_0": "Ye ariyasaccāni vibhāvayanti,",
  "snp2.1:9.2_0": "Gambhīrapaññena sudesitāni;",
  "snp2.1:9.3_0": "Kiñcāpi te honti bhusaṁ pamattā,",
  "snp2.1:9.4_0": "Na te bhavaṁ aṭṭhamamādiyanti;",
  "snp2.1:9.5_0": "Idampi saṅghe ratanaṁ paṇītaṁ,",
  "snp2.1:9.6_0": "Etena saccena suvatthi hotu.",
  "snp2.1:10.1_0": "Sahāvassa dassanasampadāya,",
  "snp2.1:10.2_0": "Tayassu dhammā jahitā bhavanti;",
  "snp2.1:10.3_0": "Sakkāyadiṭṭhī vicikicchitañca,",
  "snp2.1:10.4_0": "Sīlabbataṁ vāpi yadatthi kiñci.",
  "snp2.1:11.1_0": "Catūhapāyehi ca vippamutto,",
  "snp2.1:11.2_0": "Chaccābhiṭhānāni abhabba kātuṁ;",
  "snp2.1:11.3_0": "Idampi saṅghe ratanaṁ paṇītaṁ,",
  "snp2.1:11.4_0": "Etena saccena suvatthi hotu.",
  "snp2.1:12.1_0": "Kiñcāpi so kamma karoti pāpakaṁ,",
  "snp2.1:12.2_0": "Kāyena vācā uda cetasā vā;",
  "snp2.1:12.3_0": "Abhabba so tassa paṭicchadāya,",
  "snp2.1:12.4_0": "Abhabbatā diṭṭhapadassa vuttā;",
  "snp2.1:12.5_0": "Idampi saṅghe ratanaṁ paṇītaṁ,",
  "snp2.1:12.6_0": "Etena saccena suvatthi hotu.",
  "snp2.1:13.1_0": "Vanappagumbe yatha phussitagge,",
  "snp2.1:13.2_0": "Gimhānamāse paṭhamasmiṁ gimhe;",
  "snp2.1:13.3_0": "Tathūpamaṁ dhammavaraṁ adesayi,",
  "snp2.1:13.4_0": "Nibbānagāmiṁ paramaṁ hitāya;",
  "snp2.1:13.5_0": "Idampi buddhe ratanaṁ paṇītaṁ,",
  "snp2.1:13.6_0": "Etena saccena suvatthi hotu.",
  "snp2.1:14.1_0": "Varo varaññū varado varāharo,",
  "snp2.1:14.2_0": "Anuttaro dhammavaraṁ adesayi;",
  "snp2.1:14.3_0": "Idampi buddhe ratanaṁ paṇītaṁ,",
  "snp2.1:14.4_0": "Etena saccena suvatthi hotu.",
  "snp2.1:15.1_0": "Khīṇaṁ purāṇaṁ nava natthi sambhavaṁ,",
  "snp2.1:15.2_0": "Virattacittāyatike bhavasmiṁ;",
  "snp2.1:15.3_0": "Te khīṇabījā avirūḷhichandā,",
  "snp2.1:15.4_0": "Nibbanti dhīrā yathāyaṁ padīpo;",
  "snp2.1:15.5_0": "Idampi saṅghe ratanaṁ paṇītaṁ,",
  "snp2.1:15.6_0": "Etena saccena suvatthi hotu.",
  "snp2.1:16.1_0": "Yānīdha bhūtāni samāgatāni,",
  "snp2.1:16.2_0": "Bhummāni vā yāni va antalikkhe;",
  "snp2.1:16.3_0": "Tathāgataṁ devamanussapūjitaṁ,",
  "snp2.1:16.4_0": "Buddhaṁ namassāma suvatthi hotu.",
  "snp2.1:17.1_0": "Yānīdha bhūtāni samāgatāni,",
  "snp2.1:17.2_0": "Bhummāni vā yāni va antalikkhe;",
  "snp2.1:17.3_0": "Tathāgataṁ devamanussapūjitaṁ,",
  "snp2.1:17.4_0": "Dhammaṁ namassāma suvatthi hotu.",
  "snp2.1:18.1_0": "Yānīdha bhūtāni samāgatāni,",
  "snp2.1:18.2_0": "Bhummāni vā yāni va antalikkhe;",
  "snp2.1:18.3_0": "Tathāgataṁ devamanussapūjitaṁ,",
  "snp2.1:18.4_0": "Saṅghaṁ namassāma suvatthi hotūti.",
  "snp2.1:18.5_0": "Ratanasuttaṁ paṭhamaṁ."
}