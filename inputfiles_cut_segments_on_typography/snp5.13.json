{
  "snp5.13:0.1_0": "Sutta Nipāta 5.13",
  "snp5.13:0.2_0": "Bhadrāvudhamāṇavapucchā",
  "snp5.13:1.1_0": "“Okañjahaṁ taṇhacchidaṁ anejaṁ,",
  "snp5.13:1.2_0": "(iccāyasmā bhadrāvudho)",
  "snp5.13:1.3_0": "Nandiñjahaṁ oghatiṇṇaṁ vimuttaṁ;",
  "snp5.13:1.4_0": "Kappañjahaṁ abhiyāce sumedhaṁ,",
  "snp5.13:1.5_0": "Sutvāna nāgassa apanamissanti ito.",
  "snp5.13:2.1_0": "Nānājanā janapadehi saṅgatā,",
  "snp5.13:2.2_0": "Tava vīra vākyaṁ abhikaṅkhamānā;",
  "snp5.13:2.3_0": "Tesaṁ tuvaṁ sādhu viyākarohi,",
  "snp5.13:2.4_0": "Tathā hi te vidito esa dhammo”.",
  "snp5.13:3.1_0": "“Ādānataṇhaṁ vinayetha sabbaṁ,",
  "snp5.13:3.2_0": "(bhadrāvudhāti bhagavā",
  "snp5.13:3.3_0": "Uddhaṁ adho tiriyañcāpi majjhe;",
  "snp5.13:3.4_0": "Yaṁ yañhi lokasmimupādiyanti,",
  "snp5.13:3.5_0": "Teneva māro anveti jantuṁ.",
  "snp5.13:4.1_0": "Tasmā pajānaṁ na upādiyetha,",
  "snp5.13:4.2_0": "Bhikkhu sato kiñcanaṁ sabbaloke;",
  "snp5.13:4.3_0": "Ādānasatte iti pekkhamāno,",
  "snp5.13:4.4_0": "Pajaṁ imaṁ maccudheyye visattan”ti.",
  "snp5.13:4.5_0": "Bhadrāvudhamāṇavapucchā dvādasamā."
}