{
  "vv75:0.1_0": "Vimānavatthu",
  "vv75:0.2_0": "Purisavimāna",
  "vv75:0.3_0": "Sunikkhittavagga",
  "vv75:0.4_0": "1. Cittalatāvimānavatthu",
  "vv75:1.1_0": "“Yathā vanaṁ cittalataṁ pabhāsati,",
  "vv75:1.2_0": "Uyyānaseṭṭhaṁ tidasānamuttamaṁ;",
  "vv75:1.3_0": "Tathūpamaṁ tuyhamidaṁ vimānaṁ,",
  "vv75:1.4_0": "Obhāsayaṁ tiṭṭhati antalikkhe.",
  "vv75:2.1_0": "Deviddhipattosi mahānubhāvo,",
  "vv75:2.2_0": "Manussabhūto kimakāsi puññaṁ;",
  "vv75:2.3_0": "Kenāsi evaṁ jalitānubhāvo,",
  "vv75:2.4_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv75:3.1_0": "So devaputto attamano,",
  "vv75:3.2_0": "…pe…",
  "vv75:3.3_0": "yassa kammassidaṁ phalaṁ.",
  "vv75:4.1_0": "“Ahaṁ manussesu manussabhūto,",
  "vv75:4.2_0": "Daliddo atāṇo kapaṇo kammakaro ahosiṁ;",
  "vv75:4.3_0": "Jiṇṇe ca mātāpitaro abhāriṁ,",
  "vv75:4.4_0": "Piyā ca me sīlavanto ahesuṁ;",
  "vv75:4.5_0": "Annañca pānañca pasannacitto,",
  "vv75:4.6_0": "Sakkacca dānaṁ vipulaṁ adāsiṁ.",
  "vv75:5.1_0": "Tena metādiso vaṇṇo,",
  "vv75:5.2_0": "…pe…",
  "vv75:5.3_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv75:6.1_0": "Cittalatāvimānaṁ paṭhamaṁ."
}