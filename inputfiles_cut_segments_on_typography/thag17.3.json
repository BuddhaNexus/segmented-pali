{
  "thag17.3:0.1_0": "Theragāthā 17.3",
  "thag17.3:0.2_0": "Tiṁsanipāta",
  "thag17.3:0.3_0": "Paṭhamavagga",
  "thag17.3:0.4_0": "Ānandattheragāthā",
  "thag17.3:1.1_0": "“Pisuṇena ca kodhanena ca,",
  "thag17.3:1.2_0": "Maccharinā ca vibhūtanandinā;",
  "thag17.3:1.3_0": "Sakhitaṁ na kareyya paṇḍito,",
  "thag17.3:1.4_0": "Pāpo kāpurisena saṅgamo.",
  "thag17.3:2.1_0": "Saddhena ca pesalena ca,",
  "thag17.3:2.2_0": "Paññavatā bahussutena ca;",
  "thag17.3:2.3_0": "Sakhitaṁ kareyya paṇḍito,",
  "thag17.3:2.4_0": "Bhaddo sappurisena saṅgamo.",
  "thag17.3:3.1_0": "Passa cittakataṁ bimbaṁ,",
  "thag17.3:3.2_0": "… pe",
  "thag17.3:3.4_0": "yassa natthi dhuvaṁ ṭhiti.",
  "thag17.3:4.1_0": "Passa cittakataṁ bimbaṁ,",
  "thag17.3:4.2_0": "… pe",
  "thag17.3:4.4_0": "vatthehi sobhati.",
  "thag17.3:5.1_0": "Alattakakatā,",
  "thag17.3:5.2_0": "… pe",
  "thag17.3:5.4_0": "no ca pāragavesino.",
  "thag17.3:6.1_0": "Aṭṭhapadakatā,",
  "thag17.3:6.2_0": "… pe",
  "thag17.3:6.4_0": "no ca pāragavesino.",
  "thag17.3:7.1_0": "Añjanīva navā,",
  "thag17.3:7.2_0": "… pe",
  "thag17.3:7.4_0": "no ca pāragavesino.",
  "thag17.3:8.1_0": "Bahussuto cittakathī,",
  "thag17.3:8.2_0": "buddhassa paricārako;",
  "thag17.3:8.3_0": "Pannabhāro visaññutto,",
  "thag17.3:8.4_0": "seyyaṁ kappeti gotamo.",
  "thag17.3:9.1_0": "Khīṇāsavo visaññutto,",
  "thag17.3:9.2_0": "saṅgātīto sunibbuto;",
  "thag17.3:9.3_0": "Dhāreti antimaṁ dehaṁ,",
  "thag17.3:9.4_0": "jātimaraṇapāragū.",
  "thag17.3:10.1_0": "Yasmiṁ patiṭṭhitā dhammā,",
  "thag17.3:10.2_0": "buddhassādiccabandhuno;",
  "thag17.3:10.3_0": "Nibbānagamane magge,",
  "thag17.3:10.4_0": "soyaṁ tiṭṭhati gotamo.",
  "thag17.3:11.1_0": "Dvāsīti buddhato gaṇhiṁ,",
  "thag17.3:11.2_0": "dve sahassāni bhikkhuto;",
  "thag17.3:11.3_0": "Caturāsītisahassāni,",
  "thag17.3:11.4_0": "ye me dhammā pavattino.",
  "thag17.3:12.1_0": "Appassutāyaṁ puriso,",
  "thag17.3:12.2_0": "balibaddova jīrati;",
  "thag17.3:12.3_0": "Maṁsāni tassa vaḍḍhanti,",
  "thag17.3:12.4_0": "paññā tassa na vaḍḍhati.",
  "thag17.3:13.1_0": "Bahussuto appassutaṁ,",
  "thag17.3:13.2_0": "yo sutenātimaññati;",
  "thag17.3:13.3_0": "Andho padīpadhārova,",
  "thag17.3:13.4_0": "tatheva paṭibhāti maṁ.",
  "thag17.3:14.1_0": "Bahussutaṁ upāseyya,",
  "thag17.3:14.2_0": "sutañca na vināsaye;",
  "thag17.3:14.3_0": "Taṁ mūlaṁ brahmacariyassa,",
  "thag17.3:14.4_0": "tasmā dhammadharo siyā.",
  "thag17.3:15.1_0": "Pubbāparaññū atthaññū,",
  "thag17.3:15.2_0": "niruttipadakovido;",
  "thag17.3:15.3_0": "Suggahītañca gaṇhāti,",
  "thag17.3:15.4_0": "atthañcopaparikkhati.",
  "thag17.3:16.1_0": "Khantyā chandikato hoti,",
  "thag17.3:16.2_0": "ussahitvā tuleti taṁ;",
  "thag17.3:16.3_0": "Samaye so padahati,",
  "thag17.3:16.4_0": "ajjhattaṁ susamāhito.",
  "thag17.3:17.1_0": "Bahussutaṁ dhammadharaṁ,",
  "thag17.3:17.2_0": "sappaññaṁ buddhasāvakaṁ;",
  "thag17.3:17.3_0": "Dhammaviññāṇamākaṅkhaṁ,",
  "thag17.3:17.4_0": "taṁ bhajetha tathāvidhaṁ.",
  "thag17.3:18.1_0": "Bahussuto dhammadharo,",
  "thag17.3:18.2_0": "kosārakkho mahesino;",
  "thag17.3:18.3_0": "Cakkhu sabbassa lokassa,",
  "thag17.3:18.4_0": "pūjanīyo bahussuto.",
  "thag17.3:19.1_0": "Dhammārāmo dhammarato,",
  "thag17.3:19.2_0": "dhammaṁ anuvicintayaṁ;",
  "thag17.3:19.3_0": "Dhammaṁ anussaraṁ bhikkhu,",
  "thag17.3:19.4_0": "saddhammā na parihāyati.",
  "thag17.3:20.1_0": "Kāyamaccheragaruno,",
  "thag17.3:20.2_0": "hiyyamāne anuṭṭhahe;",
  "thag17.3:20.3_0": "Sarīrasukhagiddhassa,",
  "thag17.3:20.4_0": "kuto samaṇaphāsutā.",
  "thag17.3:21.1_0": "Na pakkhanti disā sabbā,",
  "thag17.3:21.2_0": "dhammā na paṭibhanti maṁ;",
  "thag17.3:21.3_0": "Gate kalyāṇamittamhi,",
  "thag17.3:21.4_0": "andhakāraṁva khāyati.",
  "thag17.3:22.1_0": "Abbhatītasahāyassa,",
  "thag17.3:22.2_0": "atītagatasatthuno;",
  "thag17.3:22.3_0": "Natthi etādisaṁ mittaṁ,",
  "thag17.3:22.4_0": "yathā kāyagatā sati.",
  "thag17.3:23.1_0": "Ye purāṇā atītā te,",
  "thag17.3:23.2_0": "navehi na sameti me;",
  "thag17.3:23.3_0": "Svajja ekova jhāyāmi,",
  "thag17.3:23.4_0": "vassupetova pakkhimā”.",
  "thag17.3:24.1_0": "“Dassanāya abhikkante,",
  "thag17.3:24.2_0": "nānāverajjake bahū;",
  "thag17.3:24.3_0": "Mā vārayittha sotāro,",
  "thag17.3:24.4_0": "passantu samayo mamaṁ”.",
  "thag17.3:25.1_0": "“Dassanāya abhikkante,",
  "thag17.3:25.2_0": "nānāverajjake puthu;",
  "thag17.3:25.3_0": "Karoti satthā okāsaṁ,",
  "thag17.3:25.4_0": "na nivāreti cakkhumā.",
  "thag17.3:26.1_0": "Paṇṇavīsativassāni,",
  "thag17.3:26.2_0": "sekhabhūtassa me sato;",
  "thag17.3:26.3_0": "Na kāmasaññā uppajji,",
  "thag17.3:26.4_0": "passa dhammasudhammataṁ.",
  "thag17.3:27.1_0": "Paṇṇavīsativassāni,",
  "thag17.3:27.2_0": "sekhabhūtassa me sato;",
  "thag17.3:27.3_0": "Na dosasaññā uppajji,",
  "thag17.3:27.4_0": "passa dhammasudhammataṁ.",
  "thag17.3:28.1_0": "Paṇṇavīsativassāni,",
  "thag17.3:28.2_0": "bhagavantaṁ upaṭṭhahiṁ;",
  "thag17.3:28.3_0": "Mettena kāyakammena,",
  "thag17.3:28.4_0": "chāyāva anapāyinī.",
  "thag17.3:29.1_0": "Paṇṇavīsativassāni,",
  "thag17.3:29.2_0": "bhagavantaṁ upaṭṭhahiṁ;",
  "thag17.3:29.3_0": "Mettena vacīkammena,",
  "thag17.3:29.4_0": "chāyāva anapāyinī.",
  "thag17.3:30.1_0": "Paṇṇavīsativassāni,",
  "thag17.3:30.2_0": "bhagavantaṁ upaṭṭhahiṁ;",
  "thag17.3:30.3_0": "Mettena manokammena,",
  "thag17.3:30.4_0": "chāyāva anapāyinī.",
  "thag17.3:31.1_0": "Buddhassa caṅkamantassa,",
  "thag17.3:31.2_0": "piṭṭhito anucaṅkamiṁ;",
  "thag17.3:31.3_0": "Dhamme desiyamānamhi,",
  "thag17.3:31.4_0": "ñāṇaṁ me udapajjatha.",
  "thag17.3:32.1_0": "Ahaṁ sakaraṇīyomhi,",
  "thag17.3:32.2_0": "sekho appattamānaso;",
  "thag17.3:32.3_0": "Satthu ca parinibbānaṁ,",
  "thag17.3:32.4_0": "yo amhaṁ anukampako.",
  "thag17.3:33.1_0": "Tadāsi yaṁ bhiṁsanakaṁ,",
  "thag17.3:33.2_0": "tadāsi lomahaṁsanaṁ;",
  "thag17.3:33.3_0": "Sabbākāravarūpete,",
  "thag17.3:33.4_0": "sambuddhe parinibbute”.",
  "thag17.3:34.1_0": "“Bahussuto dhammadharo,",
  "thag17.3:34.2_0": "kosārakkho mahesino;",
  "thag17.3:34.3_0": "Cakkhu sabbassa lokassa,",
  "thag17.3:34.4_0": "ānando parinibbuto.",
  "thag17.3:35.1_0": "Bahussuto dhammadharo,",
  "thag17.3:35.2_0": "kosārakkho mahesino;",
  "thag17.3:35.3_0": "Cakkhu sabbassa lokassa,",
  "thag17.3:35.4_0": "andhakāre tamonudo.",
  "thag17.3:36.1_0": "Gatimanto satimanto,",
  "thag17.3:36.2_0": "dhitimanto ca yo isi;",
  "thag17.3:36.3_0": "Saddhammadhārako thero,",
  "thag17.3:36.4_0": "ānando ratanākaro”.",
  "thag17.3:37.1_0": "“Pariciṇṇo mayā satthā,",
  "thag17.3:37.2_0": "kataṁ buddhassa sāsanaṁ;",
  "thag17.3:37.3_0": "Ohito garuko bhāro,",
  "thag17.3:37.4_0": "natthi dāni punabbhavo”ti.",
  "thag17.3:37.5_0": "… Ānando thero …",
  "thag17.3:37.6_0": "Tiṁsanipāto niṭṭhito.",
  "thag17.3:38.0_0": "Tatruddānaṁ",
  "thag17.3:38.1_0": "Phusso patisso ānando,",
  "thag17.3:38.2_0": "tayotime pakittitā;",
  "thag17.3:38.3_0": "Gāthāyo tattha saṅkhātā,",
  "thag17.3:38.4_0": "sataṁ pañca ca uttarīti."
}