{
  "ja30:0.1_0": "Jātaka",
  "ja30:0.2_0": "Ekakanipāta",
  "ja30:0.3_0": "Kuruṅgavagga",
  "ja30:0.4_0": "10. Munikajātaka",
  "ja30:1.1_0": "“Mā munikassa pihayi,",
  "ja30:1.2_0": "āturannāni bhuñjati;",
  "ja30:1.3_0": "Appossukko bhusaṁ khāda,",
  "ja30:1.4_0": "etaṁ dīghāyulakkhaṇan”ti.",
  "ja30:2.1_0": "Munikajātakaṁ dasamaṁ.",
  "ja30:2.2_0": "Kuruṅgavaggo tatiyo.",
  "ja30:3.1_0": "Tassuddānaṁ",
  "ja30:4.1_0": "Kuruṅgassa kukkuragojavaro,",
  "ja30:4.2_0": "Puna vāḷavassasirivhayano;",
  "ja30:4.3_0": "Mahiḷāmukhanāmanuññavaro,",
  "ja30:4.4_0": "Vahate dhura munikena dasāti."
}