{
  "thag1.48:0.1_0": "Theragāthā 1.48",
  "thag1.48:0.2_0": "Ekakanipāta",
  "thag1.48:0.3_0": "Pañcamavagga",
  "thag1.48:0.4_0": "Sañjayattheragāthā",
  "thag1.48:1.1_0": "“Yato ahaṁ pabbajito,",
  "thag1.48:1.2_0": "Agārasmānagāriyaṁ;",
  "thag1.48:1.3_0": "Nābhijānāmi saṅkappaṁ,",
  "thag1.48:1.4_0": "Anariyaṁ dosasaṁhitan”ti.",
  "thag1.48:1.5_0": "… Sañjayo thero …."
}