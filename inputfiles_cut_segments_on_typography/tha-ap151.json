{
  "tha-ap151:0.1_0": "Therāpadāna",
  "tha-ap151:0.2_0": "Chattavagga",
  "tha-ap151:0.3_0": "9. Vaṭaṁsakiyattheraapadāna",
  "tha-ap151:1.1_0": "“Sumedho nāma nāmena,",
  "tha-ap151:1.2_0": "sayambhū aparājito;",
  "tha-ap151:1.3_0": "Vivekamanubrūhanto,",
  "tha-ap151:1.4_0": "ajjhogāhi mahāvanaṁ.",
  "tha-ap151:2.1_0": "Saḷalaṁ pupphitaṁ disvā,",
  "tha-ap151:2.2_0": "ganthitvāna vaṭaṁsakaṁ;",
  "tha-ap151:2.3_0": "Buddhassa abhiropesiṁ,",
  "tha-ap151:2.4_0": "sammukhā lokanāyakaṁ.",
  "tha-ap151:3.1_0": "Tiṁsakappasahassamhi,",
  "tha-ap151:3.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap151:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap151:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap151:4.1_0": "Ūnavīse kappasate,",
  "tha-ap151:4.2_0": "soḷasāsuṁ sunimmitā;",
  "tha-ap151:4.3_0": "Sattaratanasampannā,",
  "tha-ap151:4.4_0": "cakkavattī mahabbalā.",
  "tha-ap151:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap151:5.2_0": "…pe…",
  "tha-ap151:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap151:6.1_0": "Itthaṁ sudaṁ āyasmā vaṭaṁsakiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap151:7.1_0": "Vaṭaṁsakiyattherassāpadānaṁ navamaṁ."
}