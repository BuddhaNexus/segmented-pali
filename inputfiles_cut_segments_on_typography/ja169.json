{
  "ja169:0.1_0": "Jātaka",
  "ja169:0.2_0": "Dukanipāta",
  "ja169:0.3_0": "Santhavavagga",
  "ja169:0.4_0": "9. Arakajātaka",
  "ja169:1.1_0": "“Yo ve mettena cittena,",
  "ja169:1.2_0": "sabbalokānukampati;",
  "ja169:1.3_0": "Uddhaṁ adho ca tiriyaṁ,",
  "ja169:1.4_0": "appamāṇena sabbaso.",
  "ja169:2.1_0": "Appamāṇaṁ hitaṁ cittaṁ,",
  "ja169:2.2_0": "paripuṇṇaṁ subhāvitaṁ;",
  "ja169:2.3_0": "Yaṁ pamāṇakataṁ kammaṁ,",
  "ja169:2.4_0": "na taṁ tatrāvasissatī”ti.",
  "ja169:3.1_0": "Arakajātakaṁ navamaṁ."
}