{
  "snp2.11:0.1_0": "Sutta Nipāta 2.11",
  "snp2.11:0.2_0": "Rāhulasutta",
  "snp2.11:1.1_0": "“Kacci abhiṇhasaṁvāsā,",
  "snp2.11:1.2_0": "nāvajānāsi paṇḍitaṁ;",
  "snp2.11:1.3_0": "Ukkādhāro manussānaṁ,",
  "snp2.11:1.4_0": "kacci apacito tayā”.",
  "snp2.11:2.1_0": "“Nāhaṁ abhiṇhasaṁvāsā,",
  "snp2.11:2.2_0": "avajānāmi paṇḍitaṁ;",
  "snp2.11:2.3_0": "Ukkādhāro manussānaṁ,",
  "snp2.11:2.4_0": "niccaṁ apacito mayā”.",
  "snp2.11:3.1_0": "“Pañca kāmaguṇe hitvā,",
  "snp2.11:3.2_0": "piyarūpe manorame;",
  "snp2.11:3.3_0": "Saddhāya gharā nikkhamma,",
  "snp2.11:3.4_0": "dukkhassantakaro bhava.",
  "snp2.11:4.1_0": "Mitte bhajassu kalyāṇe,",
  "snp2.11:4.2_0": "pantañca sayanāsanaṁ;",
  "snp2.11:4.3_0": "Vivittaṁ appanigghosaṁ,",
  "snp2.11:4.4_0": "mattaññū hohi bhojane.",
  "snp2.11:5.1_0": "Cīvare piṇḍapāte ca,",
  "snp2.11:5.2_0": "paccaye sayanāsane;",
  "snp2.11:5.3_0": "Etesu taṇhaṁ mākāsi,",
  "snp2.11:5.4_0": "mā lokaṁ punarāgami.",
  "snp2.11:6.1_0": "Saṁvuto pātimokkhasmiṁ,",
  "snp2.11:6.2_0": "indriyesu ca pañcasu;",
  "snp2.11:6.3_0": "Sati kāyagatā tyatthu,",
  "snp2.11:6.4_0": "nibbidābahulo bhava.",
  "snp2.11:7.1_0": "Nimittaṁ parivajjehi,",
  "snp2.11:7.2_0": "subhaṁ rāgūpasañhitaṁ;",
  "snp2.11:7.3_0": "Asubhāya cittaṁ bhāvehi,",
  "snp2.11:7.4_0": "ekaggaṁ susamāhitaṁ.",
  "snp2.11:8.1_0": "Animittañca bhāvehi,",
  "snp2.11:8.2_0": "Mānānusayamujjaha;",
  "snp2.11:8.3_0": "Tato mānābhisamayā,",
  "snp2.11:8.4_0": "Upasanto carissatī”ti.",
  "snp2.11:9.1_0": "Itthaṁ sudaṁ bhagavā āyasmantaṁ rāhulaṁ imāhi gāthāhi abhiṇhaṁ ovadatīti.",
  "snp2.11:9.2_0": "Rāhulasuttaṁ ekādasamaṁ."
}