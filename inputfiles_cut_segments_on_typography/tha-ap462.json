{
  "tha-ap462:0.1_0": "Therāpadāna",
  "tha-ap462:0.2_0": "Jagatidāyakavagga",
  "tha-ap462:0.3_0": "10. Gandhapūjakattheraapadāna",
  "tha-ap462:1.1_0": "“Citāsu kurumānāsu,",
  "tha-ap462:1.2_0": "nānāgandhe samāhaṭe;",
  "tha-ap462:1.3_0": "Pasannacitto sumano,",
  "tha-ap462:1.4_0": "gandhamuṭṭhimapūjayiṁ.",
  "tha-ap462:2.1_0": "Satasahassito kappe,",
  "tha-ap462:2.2_0": "citakaṁ yamapūjayiṁ;",
  "tha-ap462:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap462:2.4_0": "citapūjāyidaṁ phalaṁ.",
  "tha-ap462:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap462:3.2_0": "…pe…",
  "tha-ap462:3.3_0": "viharāmi anāsavo.",
  "tha-ap462:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap462:4.2_0": "…pe…",
  "tha-ap462:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap462:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap462:5.2_0": "…pe…",
  "tha-ap462:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap462:6.1_0": "Itthaṁ sudaṁ āyasmā gandhapūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap462:7.1_0": "Gandhapūjakattherassāpadānaṁ dasamaṁ.",
  "tha-ap462:8.1_0": "Jagatidāyakavaggo chacattālīsamo.",
  "tha-ap462:9.1_0": "Tassuddānaṁ",
  "tha-ap462:10.1_0": "Jagatī morahatthī ca,",
  "tha-ap462:10.2_0": "āsanī ukkadhārako;",
  "tha-ap462:10.3_0": "Akkami vanakoraṇḍi,",
  "tha-ap462:10.4_0": "chattado jātipūjako.",
  "tha-ap462:11.1_0": "Paṭṭipupphī ca yo thero,",
  "tha-ap462:11.2_0": "dasamo gandhapūjako;",
  "tha-ap462:11.3_0": "Sattasaṭṭhi ca gāthāyo,",
  "tha-ap462:11.4_0": "gaṇitāyo vibhāvibhi."
}