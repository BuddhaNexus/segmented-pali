{
  "ja523:0.1_0": "Jātaka",
  "ja523:0.2_0": "Cattālīsanipāta",
  "ja523:0.3_0": "Tesakuṇavagga",
  "ja523:0.4_0": "3. Alambusājātaka",
  "ja523:1.1_0": "Atha bravi brahā indo,",
  "ja523:1.2_0": "vatrabhū jayataṁ pitā;",
  "ja523:1.3_0": "Devakaññaṁ parābhetvā,",
  "ja523:1.4_0": "sudhammāyaṁ alambusaṁ.",
  "ja523:2.1_0": "“Misse devā taṁ yācanti,",
  "ja523:2.2_0": "tāvatiṁsā saindakā;",
  "ja523:2.3_0": "Isippalobhane gaccha,",
  "ja523:2.4_0": "isisiṅgaṁ alambuse.",
  "ja523:3.1_0": "Purāyaṁ amhe acceti,",
  "ja523:3.2_0": "vattavā brahmacariyavā;",
  "ja523:3.3_0": "Nibbānābhirato vuddho,",
  "ja523:3.4_0": "tassa maggāni āvara”.",
  "ja523:4.1_0": "“Devarāja kimeva tvaṁ,",
  "ja523:4.2_0": "mameva tuvaṁ sikkhasi;",
  "ja523:4.3_0": "Isippalobhane gaccha,",
  "ja523:4.4_0": "santi aññāpi accharā.",
  "ja523:5.1_0": "Mādisiyo pavarā ceva,",
  "ja523:5.2_0": "asoke nandane vane;",
  "ja523:5.3_0": "Tāsampi hotu pariyāyo,",
  "ja523:5.4_0": "tāpi yantu palobhanā”.",
  "ja523:6.1_0": "“Addhā hi saccaṁ bhaṇasi,",
  "ja523:6.2_0": "santi aññāpi accharā;",
  "ja523:6.3_0": "Tādisiyo pavarā ceva,",
  "ja523:6.4_0": "asoke nandane vane.",
  "ja523:7.1_0": "Na tā evaṁ pajānanti,",
  "ja523:7.2_0": "pāricariyaṁ pumaṁ gatā;",
  "ja523:7.3_0": "Yādisaṁ tvaṁ pajānāsi,",
  "ja523:7.4_0": "nāri sabbaṅgasobhane.",
  "ja523:8.1_0": "Tvameva gaccha kalyāṇi,",
  "ja523:8.2_0": "itthīnaṁ pavarā casi;",
  "ja523:8.3_0": "Taveva vaṇṇarūpena,",
  "ja523:8.4_0": "savasamānayissasi”.",
  "ja523:9.1_0": "“Na vāhaṁ na gamissāmi,",
  "ja523:9.2_0": "devarājena pesitā;",
  "ja523:9.3_0": "Vibhemi cetaṁ āsāduṁ,",
  "ja523:9.4_0": "uggatejo hi brāhmaṇo.",
  "ja523:10.1_0": "Aneke nirayaṁ pattā,",
  "ja523:10.2_0": "isimāsādiyā janā;",
  "ja523:10.3_0": "Āpannā mohasaṁsāraṁ,",
  "ja523:10.4_0": "tasmā lomāni haṁsaye”.",
  "ja523:11.1_0": "Idaṁ vatvāna pakkāmi,",
  "ja523:11.2_0": "accharā kāmavaṇṇinī;",
  "ja523:11.3_0": "Missā missitumicchantī,",
  "ja523:11.4_0": "isisiṅgaṁ alambusā.",
  "ja523:12.1_0": "Sā ca taṁ vanamogayha,",
  "ja523:12.2_0": "isisiṅgena rakkhitaṁ;",
  "ja523:12.3_0": "Bimbajālakasañchannaṁ,",
  "ja523:12.4_0": "samantā addhayojanaṁ.",
  "ja523:13.1_0": "Pātova pātarāsamhi,",
  "ja523:13.2_0": "udaṇhasamayaṁ pati;",
  "ja523:13.3_0": "Aggiṭṭhaṁ parimajjantaṁ,",
  "ja523:13.4_0": "isisiṅgaṁ upāgami.",
  "ja523:14.1_0": "“Kā nu vijjurivābhāsi,",
  "ja523:14.2_0": "osadhī viya tārakā;",
  "ja523:14.3_0": "Vicittahatthābharaṇā,",
  "ja523:14.4_0": "āmuttamaṇikuṇḍalā.",
  "ja523:15.1_0": "Ādiccavaṇṇasaṅkāsā,",
  "ja523:15.2_0": "hemacandanagandhinī;",
  "ja523:15.3_0": "Saññatūrū mahāmāyā,",
  "ja523:15.4_0": "kumārī cārudassanā.",
  "ja523:16.1_0": "Vilaggā mudukā suddhā,",
  "ja523:16.2_0": "pādā te suppatiṭṭhitā;",
  "ja523:16.3_0": "Gamanā kāmanīyā te,",
  "ja523:16.4_0": "harantiyeva me mano.",
  "ja523:17.1_0": "Anupubbāva te ūrū,",
  "ja523:17.2_0": "nāganāsasamūpamā;",
  "ja523:17.3_0": "Vimaṭṭhā tuyhaṁ sussoṇī,",
  "ja523:17.4_0": "akkhassa phalakaṁ yathā.",
  "ja523:18.1_0": "Uppalasseva kiñjakkhā,",
  "ja523:18.2_0": "nābhi te sādhu saṇṭhitā;",
  "ja523:18.3_0": "Pūrā kaṇhañjanasseva,",
  "ja523:18.4_0": "dūrato paṭidissati.",
  "ja523:19.1_0": "Duvidhā jātā urajā,",
  "ja523:19.2_0": "avaṇṭā sādhupaccudā;",
  "ja523:19.3_0": "Payodharā apatitā,",
  "ja523:19.4_0": "aḍḍhalābusamā thanā.",
  "ja523:20.1_0": "Dīghā kambutalābhāsā,",
  "ja523:20.2_0": "gīvā eṇeyyakā yathā;",
  "ja523:20.3_0": "Paṇḍarāvaraṇā vaggu,",
  "ja523:20.4_0": "catutthamanasannibhā.",
  "ja523:21.1_0": "Uddhaggā ca adhaggā ca,",
  "ja523:21.2_0": "dumaggaparimajjitā;",
  "ja523:21.3_0": "Duvijā nelasambhūtā,",
  "ja523:21.4_0": "dantā tava sudassanā.",
  "ja523:22.1_0": "Apaṇḍarā lohitantā,",
  "ja523:22.2_0": "jiñjūkaphalasannibhā;",
  "ja523:22.3_0": "Āyatā ca visālā ca,",
  "ja523:22.4_0": "nettā tava sudassanā.",
  "ja523:23.1_0": "Nātidīghā susammaṭṭhā,",
  "ja523:23.2_0": "kanakabyā samocitā;",
  "ja523:23.3_0": "Uttamaṅgaruhā tuyhaṁ,",
  "ja523:23.4_0": "kesā candanagandhikā.",
  "ja523:24.1_0": "Yāvatā kasigorakkhā,",
  "ja523:24.2_0": "vāṇijānaṁ ca yā gati;",
  "ja523:24.3_0": "Isīnañca parakkantaṁ,",
  "ja523:24.4_0": "saññatānaṁ tapassinaṁ.",
  "ja523:25.1_0": "Na te samasamaṁ passe,",
  "ja523:25.2_0": "asmiṁ pathavimaṇḍale;",
  "ja523:25.3_0": "Ko vā tvaṁ kassa vā putto,",
  "ja523:25.4_0": "kathaṁ jānemu taṁ mayaṁ”.",
  "ja523:26.1_0": "“Na pañhakālo bhaddante,",
  "ja523:26.2_0": "kassapevaṁ gate sati;",
  "ja523:26.3_0": "Ehi samma ramissāma,",
  "ja523:26.4_0": "ubho asmākamassame;",
  "ja523:26.5_0": "Ehi taṁ upagūhissaṁ,",
  "ja523:26.6_0": "ratīnaṁ kusalo bhava”.",
  "ja523:27.1_0": "Idaṁ vatvāna pakkāmi,",
  "ja523:27.2_0": "accharā kāmavaṇṇinī;",
  "ja523:27.3_0": "Missā missitumicchantī,",
  "ja523:27.4_0": "isisiṅgaṁ alambusā.",
  "ja523:28.1_0": "So ca vegena nikkhamma,",
  "ja523:28.2_0": "Chetvā dandhaparakkamaṁ;",
  "ja523:28.3_0": "Tamuttamāsu veṇīsu,",
  "ja523:28.4_0": "Ajjhappatto parāmasi.",
  "ja523:29.1_0": "Tamudāvatta kalyāṇī,",
  "ja523:29.2_0": "palissaji susobhanā;",
  "ja523:29.3_0": "Cavitamhi brahmacariyā,",
  "ja523:29.4_0": "yathā taṁ atha tositā.",
  "ja523:30.1_0": "Manasā agamā indaṁ,",
  "ja523:30.2_0": "vasantaṁ nandane vane;",
  "ja523:30.3_0": "Tassā saṅkappamaññāya,",
  "ja523:30.4_0": "maghavā devakuñjaro.",
  "ja523:31.1_0": "Pallaṅkaṁ pahiṇī khippaṁ,",
  "ja523:31.2_0": "sovaṇṇaṁ sopavāhanaṁ;",
  "ja523:31.3_0": "Sauttaracchadapaññāsaṁ,",
  "ja523:31.4_0": "sahassapaṭiyatthataṁ.",
  "ja523:32.1_0": "Tamenaṁ tattha dhāresi,",
  "ja523:32.2_0": "ure katvāna sobhanā;",
  "ja523:32.3_0": "Yathā ekamuhuttaṁva,",
  "ja523:32.4_0": "tīṇi vassāni dhārayi.",
  "ja523:33.1_0": "Vimado tīhi vassehi,",
  "ja523:33.2_0": "pabujjhitvāna brāhmaṇo;",
  "ja523:33.3_0": "Addasāsi haritarukkhe,",
  "ja523:33.4_0": "samantā aggiyāyanaṁ.",
  "ja523:34.1_0": "Navapattavanaṁ phullaṁ,",
  "ja523:34.2_0": "kokilaggaṇaghositaṁ;",
  "ja523:34.3_0": "Samantā paviloketvā,",
  "ja523:34.4_0": "rudaṁ assūni vattayi.",
  "ja523:35.1_0": "“Na juhe na jape mante,",
  "ja523:35.2_0": "aggihuttaṁ pahāpitaṁ;",
  "ja523:35.3_0": "Ko nu me pāricariyāya,",
  "ja523:35.4_0": "pubbe cittaṁ palobhayi.",
  "ja523:36.1_0": "Araññe me viharato,",
  "ja523:36.2_0": "yo me tejā ha sambhutaṁ;",
  "ja523:36.3_0": "Nānāratnaparipūraṁ,",
  "ja523:36.4_0": "nāvaṁva gaṇhi aṇṇave”.",
  "ja523:37.1_0": "“Ahaṁ te pāricariyāya,",
  "ja523:37.2_0": "devarājena pesitā;",
  "ja523:37.3_0": "Avadhiṁ cittaṁ cittena,",
  "ja523:37.4_0": "pamādo tvaṁ na bujjhasi”.",
  "ja523:38.1_0": "“Imāni kira maṁ tāto,",
  "ja523:38.2_0": "kassapo anusāsati;",
  "ja523:38.3_0": "Kamalāsadisitthiyo,",
  "ja523:38.4_0": "tāyo bujjhesi māṇava.",
  "ja523:39.1_0": "Uregaṇḍāyo bujjhesi,",
  "ja523:39.2_0": "tāyo bujjhesi māṇava;",
  "ja523:39.3_0": "Iccānusāsi maṁ tāto,",
  "ja523:39.4_0": "yathā maṁ anukampako.",
  "ja523:40.1_0": "Tassāhaṁ vacanaṁ nākaṁ,",
  "ja523:40.2_0": "pitu vuddhassa sāsanaṁ;",
  "ja523:40.3_0": "Araññe nimmanussamhi,",
  "ja523:40.4_0": "svajja jhāyāmi ekako.",
  "ja523:41.1_0": "Sohaṁ tathā karissāmi,",
  "ja523:41.2_0": "dhiratthu jīvitena me;",
  "ja523:41.3_0": "Puna vā tādiso hessaṁ,",
  "ja523:41.4_0": "maraṇaṁ me bhavissati”.",
  "ja523:42.1_0": "Tassa tejaṁ vīriyañca,",
  "ja523:42.2_0": "Dhitiṁ ñatvā avaṭṭhitaṁ;",
  "ja523:42.3_0": "Sirasā aggahī pāde,",
  "ja523:42.4_0": "Isisiṅgaṁ alambusā.",
  "ja523:43.1_0": "“Mā me kujjha mahāvīra,",
  "ja523:43.2_0": "mā me kujjha mahāise;",
  "ja523:43.3_0": "Mahā attho mayā ciṇṇo,",
  "ja523:43.4_0": "tidasānaṁ yasassinaṁ;",
  "ja523:43.5_0": "Tayā saṅkampitaṁ āsi,",
  "ja523:43.6_0": "sabbaṁ devapuraṁ tadā”.",
  "ja523:44.1_0": "“Tāvatiṁsā ca ye devā,",
  "ja523:44.2_0": "tidasānañca vāsavo;",
  "ja523:44.3_0": "Tvañca bhadde sukhī hohi,",
  "ja523:44.4_0": "gaccha kaññe yathāsukhaṁ”.",
  "ja523:45.1_0": "Tassa pāde gahetvāna,",
  "ja523:45.2_0": "katvā ca naṁ padakkhiṇaṁ;",
  "ja523:45.3_0": "Añjaliṁ paggahetvāna,",
  "ja523:45.4_0": "tamhā ṭhānā apakkami.",
  "ja523:46.1_0": "Yo ca tassāsi pallaṅko,",
  "ja523:46.2_0": "sovaṇṇo sopavāhano;",
  "ja523:46.3_0": "Sauttaracchadapaññāso,",
  "ja523:46.4_0": "sahassapaṭiyatthato;",
  "ja523:46.5_0": "Tameva pallaṅkamāruyha,",
  "ja523:46.6_0": "agā devāna santike.",
  "ja523:47.1_0": "Tamokkamiva āyantiṁ,",
  "ja523:47.2_0": "jalantiṁ vijjutaṁ yathā;",
  "ja523:47.3_0": "Patīto sumano vitto,",
  "ja523:47.4_0": "devindo adadā varaṁ.",
  "ja523:48.1_0": "“Varañce me ado sakka,",
  "ja523:48.2_0": "sabbabhūtānamissara;",
  "ja523:48.3_0": "Nisippalobhikā gacche,",
  "ja523:48.4_0": "etaṁ sakka varaṁ vare”ti.",
  "ja523:49.1_0": "Alambusājātakaṁ tatiyaṁ."
}