{
  "iti63:0.1_0": "Itivuttaka 63",
  "iti63:0.2_0": "Tikanipāta",
  "iti63:0.3_0": "Dutiyavagga",
  "iti63:0.4_0": "Addhāsutta",
  "iti63:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti63:2.1_0": "“Tayome, bhikkhave, addhā.",
  "iti63:2.2_0": "Katame tayo?",
  "iti63:2.3_0": "Atīto addhā, anāgato addhā, paccuppanno addhā—",
  "iti63:2.4_0": "ime kho, bhikkhave, tayo addhā”ti.",
  "iti63:2.5_0": "Etamatthaṁ bhagavā avoca.",
  "iti63:2.6_0": "Tatthetaṁ iti vuccati:",
  "iti63:3.1_0": "“Akkheyyasaññino sattā,",
  "iti63:3.2_0": "akkheyyasmiṁ patiṭṭhitā;",
  "iti63:3.3_0": "Akkheyyaṁ apariññāya,",
  "iti63:3.4_0": "yogamāyanti maccuno.",
  "iti63:4.1_0": "Akkheyyañca pariññāya,",
  "iti63:4.2_0": "akkhātāraṁ na maññati;",
  "iti63:4.3_0": "Phuṭṭho vimokkho manasā,",
  "iti63:4.4_0": "santipadamanuttaraṁ.",
  "iti63:5.1_0": "Sa ve akkheyyasampanno,",
  "iti63:5.2_0": "santo santipade rato;",
  "iti63:5.3_0": "Saṅkhāya sevī dhammaṭṭho,",
  "iti63:5.4_0": "saṅkhyaṁ nopeti vedagū”ti.",
  "iti63:6.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti63:6.2_0": "Catutthaṁ."
}