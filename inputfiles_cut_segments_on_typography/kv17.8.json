{
  "kv17.8:0.1_0": "Kathāvatthu",
  "kv17.8:0.2_0": "Mahāpaṇṇāsaka",
  "kv17.8:0.3_0": "Sattarasamavagga",
  "kv17.8:0.4_0": "Navattabbaṁsaṅghobhuñjatītikathā",
  "kv17.8:1.1_0": "Na vattabbaṁ—",
  "kv17.8:1.2_0": "“saṅgho bhuñjati pivati khādati sāyatī”ti?",
  "kv17.8:1.3_0": "Āmantā.",
  "kv17.8:1.4_0": "Nanu atthi keci saṅghabhattāni karonti, uddesabhattāni karonti, yāgupānāni karontīti?",
  "kv17.8:1.5_0": "Āmantā.",
  "kv17.8:1.6_0": "Hañci atthi keci saṅghabhattāni karonti,",
  "kv17.8:1.6_1": "uddesabhattāni karonti,",
  "kv17.8:1.6_2": "yāgupānāni karonti,",
  "kv17.8:1.6_3": "tena vata re vattabbe—",
  "kv17.8:1.7_0": "“saṅgho bhuñjati pivati khādati sāyatī”ti.",
  "kv17.8:2.1_0": "Na vattabbaṁ—",
  "kv17.8:2.2_0": "“saṅgho bhuñjati pivati khādati sāyatī”ti?",
  "kv17.8:2.3_0": "Āmantā.",
  "kv17.8:2.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv17.8:2.5_0": "“gaṇabhojanaṁ paramparabhojanaṁ atirittabhojanaṁ anatirittabhojanan”ti?",
  "kv17.8:2.6_0": "Āmantā.",
  "kv17.8:2.7_0": "Hañci vuttaṁ bhagavatā—",
  "kv17.8:2.8_0": "“gaṇabhojanaṁ paramparabhojanaṁ atirittabhojanaṁ anatirittabhojanaṁ”, tena vata re vattabbe—",
  "kv17.8:2.9_0": "“saṅgho bhuñjati pivati khādati sāyatī”ti.",
  "kv17.8:3.1_0": "Na vattabbaṁ—",
  "kv17.8:3.2_0": "“saṅgho bhuñjati pivati khādati sāyatī”ti?",
  "kv17.8:3.3_0": "Āmantā.",
  "kv17.8:3.4_0": "Nanu aṭṭha pānāni vuttāni bhagavatā—",
  "kv17.8:3.5_0": "ambapānaṁ,",
  "kv17.8:3.5_1": "jambupānaṁ,",
  "kv17.8:3.5_2": "cocapānaṁ,",
  "kv17.8:3.5_3": "mocapānaṁ,",
  "kv17.8:3.5_4": "madhukapānaṁ,",
  "kv17.8:3.5_5": "muddikapānaṁ,",
  "kv17.8:3.5_6": "sālukapānaṁ,",
  "kv17.8:3.5_7": "phārusakapānanti?",
  "kv17.8:3.6_0": "Āmantā.",
  "kv17.8:3.7_0": "Hañci aṭṭha pānāni vuttāni bhagavatā—",
  "kv17.8:3.8_0": "ambapānaṁ,",
  "kv17.8:3.8_1": "jambupānaṁ,",
  "kv17.8:3.8_2": "cocapānaṁ,",
  "kv17.8:3.8_3": "mocapānaṁ,",
  "kv17.8:3.8_4": "madhukapānaṁ,",
  "kv17.8:3.8_5": "muddikapānaṁ,",
  "kv17.8:3.8_6": "sālukapānaṁ,",
  "kv17.8:3.8_7": "phārusakapānaṁ,",
  "kv17.8:3.8_8": "tena vata re vattabbe—",
  "kv17.8:3.9_0": "“saṅgho bhuñjati pivati khādati sāyatī”ti.",
  "kv17.8:4.1_0": "Saṅgho bhuñjati pivati khādati sāyatīti?",
  "kv17.8:4.2_0": "Āmantā.",
  "kv17.8:4.3_0": "Maggo bhuñjati pivati khādati sāyati, phalaṁ bhuñjati pivati khādati sāyatīti?",
  "kv17.8:4.4_0": "Na hevaṁ vattabbe …pe….",
  "kv17.8:4.5_0": "Na vattabbaṁ saṅgho bhuñjatītikathā niṭṭhitā."
}