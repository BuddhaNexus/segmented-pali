{
  "thag1.74:0.1_0": "Theragāthā 1.74",
  "thag1.74:0.2_0": "Ekakanipāta",
  "thag1.74:0.3_0": "Aṭṭhamavagga",
  "thag1.74:0.4_0": "Suyāmanattheragāthā",
  "thag1.74:1.1_0": "“Kāmacchando ca byāpādo,",
  "thag1.74:1.2_0": "Thinamiddhañca bhikkhuno;",
  "thag1.74:1.3_0": "Uddhaccaṁ vicikicchā ca,",
  "thag1.74:1.4_0": "Sabbasova na vijjatī”ti.",
  "thag1.74:1.5_0": "… Suyāmano thero …."
}