{
  "ja253:0.1_0": "Jātaka",
  "ja253:0.2_0": "Tikanipāta",
  "ja253:0.3_0": "Saṅkappavagga",
  "ja253:0.4_0": "3. Maṇikaṇṭhajātaka",
  "ja253:1.1_0": "“Mamannapānaṁ vipulaṁ uḷāraṁ,",
  "ja253:1.2_0": "Uppajjatīmassa maṇissa hetu;",
  "ja253:1.3_0": "Taṁ te na dassaṁ atiyācakosi,",
  "ja253:1.4_0": "Na cāpi te assamamāgamissaṁ.",
  "ja253:2.1_0": "Susū yathā sakkharadhotapāṇī,",
  "ja253:2.2_0": "Tāsesi maṁ selaṁ yācamāno;",
  "ja253:2.3_0": "Taṁ te na dassaṁ atiyācakosi,",
  "ja253:2.4_0": "Na cāpi te assamamāgamissaṁ”.",
  "ja253:3.1_0": "“Na taṁ yāce yassa piyaṁ jigīse,",
  "ja253:3.2_0": "Desso hoti atiyācanāya;",
  "ja253:3.3_0": "Nāgo maṇiṁ yācito brāhmaṇena,",
  "ja253:3.4_0": "Adassanaṁyeva tadajjhagamā”ti.",
  "ja253:4.1_0": "Maṇikaṇṭhajātakaṁ tatiyaṁ."
}