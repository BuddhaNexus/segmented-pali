{
  "thag2.21:0.1_0": "Theragāthā 2.21",
  "thag2.21:0.2_0": "Dukanipāta",
  "thag2.21:0.3_0": "Tatiyavagga",
  "thag2.21:0.4_0": "Uttarattheragāthā",
  "thag2.21:1.1_0": "“Khandhā mayā pariññātā,",
  "thag2.21:1.2_0": "taṇhā me susamūhatā;",
  "thag2.21:1.3_0": "Bhāvitā mama bojjhaṅgā,",
  "thag2.21:1.4_0": "patto me āsavakkhayo.",
  "thag2.21:2.1_0": "Sohaṁ khandhe pariññāya,",
  "thag2.21:2.2_0": "abbahitvāna jāliniṁ;",
  "thag2.21:2.3_0": "Bhāvayitvāna bojjhaṅge,",
  "thag2.21:2.4_0": "nibbāyissaṁ anāsavo”ti.",
  "thag2.21:2.5_0": "… Uttaro thero …."
}