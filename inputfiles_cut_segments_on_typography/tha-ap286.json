{
  "tha-ap286:0.1_0": "Therāpadāna",
  "tha-ap286:0.2_0": "Paṇṇadāyakavagga",
  "tha-ap286:0.3_0": "4. Ekapupphiyattheraapadāna",
  "tha-ap286:1.1_0": "“Dakkhiṇamhi duvāramhi,",
  "tha-ap286:1.2_0": "pisāco āsahaṁ tadā;",
  "tha-ap286:1.3_0": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap286:1.4_0": "pītaraṁsiṁva bhāṇumaṁ.",
  "tha-ap286:2.1_0": "Vipassissa naraggassa,",
  "tha-ap286:2.2_0": "sabbalokahitesino;",
  "tha-ap286:2.3_0": "Ekapupphaṁ mayā dinnaṁ,",
  "tha-ap286:2.4_0": "dvipadindassa tādino.",
  "tha-ap286:3.1_0": "Ekanavutito kappe,",
  "tha-ap286:3.2_0": "yaṁ pupphamadadiṁ tadā;",
  "tha-ap286:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap286:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap286:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap286:4.2_0": "…pe…",
  "tha-ap286:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap286:5.1_0": "Itthaṁ sudaṁ āyasmā ekapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap286:6.1_0": "Ekapupphiyattherassāpadānaṁ catutthaṁ."
}