{
  "kv3.9:0.1_0": "Kathāvatthu",
  "kv3.9:0.2_0": "Mahāpaṇṇāsaka",
  "kv3.9:0.3_0": "Tatiyavagga",
  "kv3.9:0.4_0": "Yathākammūpagatañāṇakathā",
  "kv3.9:1.1_0": "Yathākammūpagataṁ ñāṇaṁ dibbacakkhunti?",
  "kv3.9:1.2_0": "Āmantā.",
  "kv3.9:1.3_0": "Yathākammūpagatañca manasi karoti, dibbena cakkhunā rūpaṁ passatīti?",
  "kv3.9:1.4_0": "Na hevaṁ vattabbe …pe….",
  "kv3.9:2.1_0": "Yathākammūpagatañca manasi karoti, dibbena cakkhunā rūpaṁ passatīti?",
  "kv3.9:2.2_0": "Āmantā.",
  "kv3.9:2.3_0": "Dvinnaṁ phassānaṁ dvinnaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv3.9:2.4_0": "Na hevaṁ vattabbe …pe….",
  "kv3.9:3.1_0": "Yathākammūpagataṁ ñāṇaṁ dibbacakkhunti?",
  "kv3.9:3.2_0": "Āmantā.",
  "kv3.9:3.3_0": "“Ime vata bhonto sattā”ti ca manasi karoti,",
  "kv3.9:3.3_1": "“kāyaduccaritena samannāgatā”ti ca manasi karoti,",
  "kv3.9:3.3_2": "“vacīduccaritena samannāgatā”ti ca manasi karoti,",
  "kv3.9:3.3_3": "“manoduccaritena samannāgatā”ti ca manasi karoti,",
  "kv3.9:3.3_4": "“ariyānaṁ upavādakā”ti ca manasi karoti,",
  "kv3.9:3.3_5": "“micchādiṭṭhikā”ti ca manasi karoti,",
  "kv3.9:3.3_6": "“micchādiṭṭhikammasamādānā”ti ca manasi karoti,",
  "kv3.9:3.3_7": "“te kāyassa bhedā paraṁ maraṇā apāyaṁ duggatiṁ vinipātaṁ nirayaṁ upapannā”ti ca manasi karoti,",
  "kv3.9:3.3_8": "“ime vā pana bhonto sattā”ti ca manasi karoti,",
  "kv3.9:3.3_9": "“kāyasucaritena samannāgatā”ti ca manasi karoti,",
  "kv3.9:3.3_10": "“vacīsucaritena samannāgatā”ti ca manasi karoti,",
  "kv3.9:3.3_11": "“manosucaritena samannāgatā”ti ca manasi karoti,",
  "kv3.9:3.3_12": "“ariyānaṁ anupavādakā”ti ca manasi karoti,",
  "kv3.9:3.3_13": "“sammādiṭṭhikā”ti ca manasi karoti,",
  "kv3.9:3.3_14": "“sammādiṭṭhikammasamādānā”ti ca manasi karoti,",
  "kv3.9:3.3_15": "“te kāyassa bhedā paraṁ maraṇā sugatiṁ saggaṁ lokaṁ upapannā”ti ca manasi karoti,",
  "kv3.9:3.3_16": "dibbena cakkhunā rūpaṁ passatīti?",
  "kv3.9:3.4_0": "Na hevaṁ vattabbe …pe….",
  "kv3.9:4.1_0": "“Te kāyassa bhedā paraṁ maraṇā sugatiṁ saggaṁ lokaṁ upapannā”ti ca manasi karoti,",
  "kv3.9:4.1_1": "dibbena cakkhunā rūpaṁ passatīti?",
  "kv3.9:4.2_0": "Āmantā.",
  "kv3.9:4.3_0": "Dvinnaṁ phassānaṁ dvinnaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv3.9:4.4_0": "Na hevaṁ vattabbe …pe….",
  "kv3.9:5.1_0": "Yathākammūpagataṁ ñāṇaṁ dibbacakkhunti?",
  "kv3.9:5.2_0": "Āmantā.",
  "kv3.9:5.3_0": "Atthi koci adibbacakkhuko dibbacakkhuṁ appaṭiladdho anadhigato asacchikato yathākammūpagataṁ jānātīti?",
  "kv3.9:5.4_0": "Āmantā.",
  "kv3.9:5.5_0": "Hañci atthi koci adibbacakkhuko dibbacakkhuṁ appaṭiladdho anadhigato asacchikato yathākammūpagataṁ jānāti,",
  "kv3.9:5.5_1": "no ca vata re vattabbe—",
  "kv3.9:5.6_0": "“yathākammūpagataṁ ñāṇaṁ dibbacakkhun”ti.",
  "kv3.9:6.1_0": "Yathākammūpagataṁ ñāṇaṁ dibbacakkhunti?",
  "kv3.9:6.2_0": "Āmantā.",
  "kv3.9:6.3_0": "Āyasmā sāriputto yathākammūpagataṁ ñāṇaṁ jānātīti?",
  "kv3.9:6.4_0": "Āmantā.",
  "kv3.9:6.5_0": "Hañci āyasmā sāriputto yathākammūpagataṁ ñāṇaṁ jānāti, no ca vata re vattabbe—",
  "kv3.9:6.6_0": "“yathākammūpagataṁ ñāṇaṁ dibbacakkhun”ti.",
  "kv3.9:7.1_0": "Yathākammūpagataṁ ñāṇaṁ dibbacakkhunti?",
  "kv3.9:7.2_0": "Āmantā.",
  "kv3.9:7.3_0": "Āyasmā sāriputto yathākammūpagataṁ ñāṇaṁ jānātīti?",
  "kv3.9:7.4_0": "Āmantā.",
  "kv3.9:7.5_0": "Atthāyasmato sāriputtassa dibbacakkhunti?",
  "kv3.9:7.6_0": "Na hevaṁ vattabbe …pe….",
  "kv3.9:8.1_0": "Atthāyasmato sāriputtassa dibbacakkhunti?",
  "kv3.9:8.2_0": "Āmantā.",
  "kv3.9:8.3_0": "Nanu āyasmā sāriputto etadavoca—",
  "kv3.9:9.1_0": "“Neva pubbenivāsāya,",
  "kv3.9:9.2_0": "napi dibbassa cakkhuno;",
  "kv3.9:9.3_0": "Cetopariyāya iddhiyā,",
  "kv3.9:9.4_0": "sotadhātuvisuddhiyā;",
  "kv3.9:9.5_0": "Cutiyā upapattiyā,",
  "kv3.9:9.6_0": "paṇidhi me na vijjatī”ti.",
  "kv3.9:10.1_0": "Attheva suttantoti?",
  "kv3.9:10.2_0": "Āmantā.",
  "kv3.9:10.3_0": "Tena hi na vattabbaṁ—",
  "kv3.9:10.4_0": "“yathākammūpagataṁ ñāṇaṁ dibbacakkhun”ti.",
  "kv3.9:10.5_0": "Yathākammūpagatañāṇakathā niṭṭhitā."
}