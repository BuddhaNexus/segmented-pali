{
  "atk-s0403a33:0_0": "8. Arahattavaggo",
  "atk-s0403a33:1_0": "1. Dukkhasuttavaṇṇanā",
  "atk-s0403a33:2_0": "75.",
  "atk-s0403a33:2_1": "Aṭṭhamassa paṭhame savighātanti saupaghātaṁ sopaddavaṁ.",
  "atk-s0403a33:2_2": "Sapariḷāhanti kāyikacetasikena pariḷāhena sapariḷāhaṁ.",
  "atk-s0403a33:2_3": "Pāṭikaṅkhāti icchitabbā avassaṁbhāvinī.",
  "atk-s0403a33:3_0": "2. Arahattasuttavaṇṇanā",
  "atk-s0403a33:4_0": "76.",
  "atk-s0403a33:4_1": "Dutiye mānanti jātiādīhi maññanaṁ.",
  "atk-s0403a33:4_2": "Omānanti hīnohamasmīti mānaṁ.",
  "atk-s0403a33:4_3": "Atimānanti atikkamitvā pavattaṁ accuṇṇatimānaṁ.",
  "atk-s0403a33:4_4": "Adhimānanti adhigatamānaṁ.",
  "atk-s0403a33:4_5": "Thambhanti kodhamānehi thaddhabhāvaṁ.",
  "atk-s0403a33:4_6": "Atinipātanti hīnassa hīnohamasmīti mānaṁ.",
  "atk-s0403a33:5_0": "3. Uttarimanussadhammasuttavaṇṇanā",
  "atk-s0403a33:6_0": "77.",
  "atk-s0403a33:6_1": "Tatiye uttarimanussadhammāti manussadhammato uttari.",
  "atk-s0403a33:6_2": "Alamariyañāṇadassanavisesanti ariyabhāvaṁ kātuṁ samatthaṁ ñāṇadassanavisesaṁ,",
  "atk-s0403a33:6_3": "cattāro magge cattāri ca phalānīti attho.",
  "atk-s0403a33:6_4": "Kuhananti tividhaṁ kuhanavatthuṁ.",
  "atk-s0403a33:6_5": "Lapananti lābhatthikatāya ukkhipitvā avakkhipitvā vā lapanaṁ.",
  "atk-s0403a33:7_0": "4. Sukhasomanassasuttavaṇṇanā",
  "atk-s0403a33:8_0": "78.",
  "atk-s0403a33:8_1": "Catutthe yoni cassa āraddhā hotīti kāraṇañcassa paripuṇṇaṁ paggahitaṁ hoti.",
  "atk-s0403a33:8_2": "Dhammārāmoti dhamme ratiṁ vindati.",
  "atk-s0403a33:8_3": "Bhāvanāya ramati,",
  "atk-s0403a33:8_4": "bhāvento vā ramatīti bhāvanārāmo.",
  "atk-s0403a33:8_5": "Pahāne ramati,",
  "atk-s0403a33:8_6": "pajahanto vā ramatīti pahānārāmo.",
  "atk-s0403a33:8_7": "Tividhe paviveke ramatīti pavivekārāmo.",
  "atk-s0403a33:8_8": "Abyāpajjhe niddukkhabhāve ramatīti abyāpajjhārāmo.",
  "atk-s0403a33:8_9": "Nippapañcasaṅkhāte nibbāne ramatīti nippapañcārāmo.",
  "atk-s0403a33:9_0": "5. Adhigamasuttavaṇṇanā",
  "atk-s0403a33:10_0": "79.",
  "atk-s0403a33:10_1": "Pañcame na āyakusaloti na āgamanakusalo.",
  "atk-s0403a33:10_2": "Na apāyakusaloti na apagamanakusalo.",
  "atk-s0403a33:10_3": "Chandanti kattukamyatāchandaṁ.",
  "atk-s0403a33:10_4": "Na ārakkhatīti na rakkhati.",
  "atk-s0403a33:11_0": "6. Mahantattasuttavaṇṇanā",
  "atk-s0403a33:12_0": "80.",
  "atk-s0403a33:12_1": "Chaṭṭhe ālokabahuloti ñāṇālokabahulo.",
  "atk-s0403a33:12_2": "Yogabahuloti yoge bahulaṁ karoti.",
  "atk-s0403a33:12_3": "Vedabahuloti pītipāmojjabahulo.",
  "atk-s0403a33:12_4": "Asantuṭṭhibahuloti kusaladhammesu asantuṭṭho.",
  "atk-s0403a33:12_5": "Anikkhittadhuroti aṭṭhapitadhuro paggahitavīriyo.",
  "atk-s0403a33:12_6": "Uttari ca patāretīti sampati ca uttariñca vīriyaṁ karoteva.",
  "atk-s0403a33:12_7": "Sattamaṁ uttānameva.",
  "atk-s0403a33:13_0": "8-10. Dutiyanirayasuttādivaṇṇanā",
  "atk-s0403a33:14_0": "82- 84.",
  "atk-s0403a33:14_1": "Aṭṭhame pagabbhoti kāyapāgabbhiyādīhi samannāgato.",
  "atk-s0403a33:14_2": "Navamaṁ uttānatthameva.",
  "atk-s0403a33:14_3": "Dasame vighātavāti mahicchataṁ nissāya uppannena lobhadukkhena dukkhito.",
  "atk-s0403a33:14_4": "Sesaṁ sabbattha uttānamevāti.",
  "atk-s0403a33:15_0": "Arahattavaggo aṭṭhamo."
}