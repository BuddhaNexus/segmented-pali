{
  "tika-s0404t26:0_0": "1. Nissayavaggo",
  "tika-s0404t26:1_0": "1-10. Kimatthiyasuttādivaṇṇanā",
  "tika-s0404t26:2_0": "1- 10.",
  "tika-s0404t26:2_1": "Ekādasakanipātassa paṭhamādīni uttānatthāneva.",
  "tika-s0404t26:2_2": "Dasame janitasminti kammakilesehi nibbatte,",
  "tika-s0404t26:2_3": "jane etasminti vā janetasmiṁ,",
  "tika-s0404t26:2_4": "manussesūti attho.",
  "tika-s0404t26:2_5": "Tenāha “ye gottapaṭisārino”ti.",
  "tika-s0404t26:2_6": "Janitasmiṁ-",
  "tika-s0404t26:2_7": "saddo eva vā i-",
  "tika-s0404t26:2_8": "kārassa e-",
  "tika-s0404t26:2_9": "kāraṁ katvā “janetasmi”nti vutto.",
  "tika-s0404t26:2_10": "Janitasminti ca janasminti attho veditabbo.",
  "tika-s0404t26:2_11": "Janitasminti sāmaññaggahaṇepi yattha catuvaṇṇasamaññā,",
  "tika-s0404t26:2_12": "tattheva manussaloke.",
  "tika-s0404t26:2_13": "Khattiyo seṭṭhoti ayaṁ lokasamaññāpi manussalokeyeva,",
  "tika-s0404t26:2_14": "na devakāye brahmakāye vāti dassetuṁ “ye gottapaṭisārino”ti vuttaṁ.",
  "tika-s0404t26:2_15": "Paṭisarantīti “ahaṁ gotamo,",
  "tika-s0404t26:2_16": "ahaṁ kassapo”ti paṭi paṭi attano gottaṁ anussaranti paṭijānanti vāti attho.",
  "tika-s0404t26:3_0": "Kimatthiyasuttādivaṇṇanā niṭṭhitā.",
  "tika-s0404t26:4_0": "Nissayavaggavaṇṇanā niṭṭhitā."
}