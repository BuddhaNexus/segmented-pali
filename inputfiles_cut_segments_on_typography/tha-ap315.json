{
  "tha-ap315:0.1_0": "Therāpadāna",
  "tha-ap315:0.2_0": "Ārakkhadāyakavagga",
  "tha-ap315:0.3_0": "3. Gatasaññakattheraapadāna",
  "tha-ap315:1.1_0": "“Ākāseva padaṁ natthi,",
  "tha-ap315:1.2_0": "ambare anilañjase;",
  "tha-ap315:1.3_0": "Siddhatthaṁ jinamaddakkhiṁ,",
  "tha-ap315:1.4_0": "gacchantaṁ tidivaṅgaṇe.",
  "tha-ap315:2.1_0": "Anileneritaṁ disvā,",
  "tha-ap315:2.2_0": "sammāsambuddhacīvaraṁ;",
  "tha-ap315:2.3_0": "Vitti mamāhu tāvade,",
  "tha-ap315:2.4_0": "disvāna gamanaṁ muniṁ.",
  "tha-ap315:3.1_0": "Catunnavutito kappe,",
  "tha-ap315:3.2_0": "yaṁ saññamalabhiṁ tadā;",
  "tha-ap315:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap315:3.4_0": "buddhasaññāyidaṁ phalaṁ.",
  "tha-ap315:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap315:4.2_0": "…pe…",
  "tha-ap315:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap315:5.1_0": "Itthaṁ sudaṁ āyasmā gatasaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap315:6.1_0": "Gatasaññakattherassāpadānaṁ tatiyaṁ."
}