{
  "thag1.22:0.1_0": "Theragāthā 1.22",
  "thag1.22:0.2_0": "Ekakanipāta",
  "thag1.22:0.3_0": "Tatiyavagga",
  "thag1.22:0.4_0": "Cittakattheragāthā",
  "thag1.22:1.1_0": "“Nīlā sugīvā sikhino,",
  "thag1.22:1.2_0": "Morā kārambhiyaṁ abhinadanti;",
  "thag1.22:1.3_0": "Te sītavātakīḷitā,",
  "thag1.22:1.4_0": "Suttaṁ jhāyaṁ nibodhentī”ti.",
  "thag1.22:1.5_0": "… Cittako thero …."
}