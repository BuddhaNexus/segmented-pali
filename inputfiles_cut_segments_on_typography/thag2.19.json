{
  "thag2.19:0.1_0": "Theragāthā 2.19",
  "thag2.19:0.2_0": "Dukanipāta",
  "thag2.19:0.3_0": "Dutiyavagga",
  "thag2.19:0.4_0": "Nandattheragāthā",
  "thag2.19:1.1_0": "“Ayoniso manasikārā,",
  "thag2.19:1.2_0": "maṇḍanaṁ anuyuñjisaṁ;",
  "thag2.19:1.3_0": "Uddhato capalo cāsiṁ,",
  "thag2.19:1.4_0": "kāmarāgena aṭṭito.",
  "thag2.19:2.1_0": "Upāyakusalenāhaṁ,",
  "thag2.19:2.2_0": "buddhenādiccabandhunā;",
  "thag2.19:2.3_0": "Yoniso paṭipajjitvā,",
  "thag2.19:2.4_0": "bhave cittaṁ udabbahin”ti.",
  "thag2.19:2.5_0": "… Nando thero …."
}