{
  "tha-ap347:0.1_0": "Therāpadāna",
  "tha-ap347:0.2_0": "Ekapadumiyavagga",
  "tha-ap347:0.3_0": "5. Naḷāgārikattheraapadāna",
  "tha-ap347:1.1_0": "“Himavantassāvidūre,",
  "tha-ap347:1.2_0": "hārito nāma pabbato;",
  "tha-ap347:1.3_0": "Sayambhū nārado nāma,",
  "tha-ap347:1.4_0": "rukkhamūle vasī tadā.",
  "tha-ap347:2.1_0": "Naḷāgāraṁ karitvāna,",
  "tha-ap347:2.2_0": "tiṇena chādayiṁ ahaṁ;",
  "tha-ap347:2.3_0": "Caṅkamaṁ sodhayitvāna,",
  "tha-ap347:2.4_0": "sayambhussa adāsahaṁ.",
  "tha-ap347:3.1_0": "Catuddasasu kappesu,",
  "tha-ap347:3.2_0": "devaloke ramiṁ ahaṁ;",
  "tha-ap347:3.3_0": "Catusattatikkhattuñca,",
  "tha-ap347:3.4_0": "devarajjaṁ akārayiṁ.",
  "tha-ap347:4.1_0": "Catusattatikkhattuñca,",
  "tha-ap347:4.2_0": "cakkavattī ahosahaṁ;",
  "tha-ap347:4.3_0": "Padesarajjaṁ vipulaṁ,",
  "tha-ap347:4.4_0": "gaṇanāto asaṅkhiyaṁ.",
  "tha-ap347:5.1_0": "Ubbiddhaṁ bhavanaṁ mayhaṁ,",
  "tha-ap347:5.2_0": "Indalaṭṭhīva uggataṁ;",
  "tha-ap347:5.3_0": "Sahassathambhaṁ atulaṁ,",
  "tha-ap347:5.4_0": "Vimānaṁ sapabhassaraṁ.",
  "tha-ap347:6.1_0": "Dve sampattī anubhotvā,",
  "tha-ap347:6.2_0": "sukkamūlena codito;",
  "tha-ap347:6.3_0": "Gotamassa bhagavato,",
  "tha-ap347:6.4_0": "sāsane pabbajiṁ ahaṁ.",
  "tha-ap347:7.1_0": "Padhānapahitattomhi,",
  "tha-ap347:7.2_0": "upasanto nirūpadhi;",
  "tha-ap347:7.3_0": "Nāgova bandhanaṁ chetvā,",
  "tha-ap347:7.4_0": "viharāmi anāsavo.",
  "tha-ap347:8.1_0": "Paṭisambhidā catasso,",
  "tha-ap347:8.2_0": "…pe…",
  "tha-ap347:8.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap347:9.1_0": "Itthaṁ sudaṁ āyasmā naḷāgāriko thero imā gāthāyo abhāsitthāti.",
  "tha-ap347:10.1_0": "Naḷāgārikattherassāpadānaṁ pañcamaṁ."
}