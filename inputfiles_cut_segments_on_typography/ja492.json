{
  "ja492:0.1_0": "Jātaka",
  "ja492:0.2_0": "Pakiṇṇakanipāta",
  "ja492:0.3_0": "Sālikedāravagga",
  "ja492:0.4_0": "9. Tacchasūkarajātaka",
  "ja492:1.1_0": "“Yadesamānā vicarimha,",
  "ja492:1.2_0": "pabbatāni vanāni ca;",
  "ja492:1.3_0": "Anvesaṁ vicariṁ ñātī,",
  "ja492:1.4_0": "teme adhigatā mayā.",
  "ja492:2.1_0": "Bahuñcidaṁ mūlaphalaṁ,",
  "ja492:2.2_0": "bhakkho cāyaṁ anappako;",
  "ja492:2.3_0": "Rammā cimā girīnajjo,",
  "ja492:2.4_0": "phāsuvāso bhavissati.",
  "ja492:3.1_0": "Idhevāhaṁ vasissāmi,",
  "ja492:3.2_0": "saha sabbehi ñātibhi;",
  "ja492:3.3_0": "Appossukko nirāsaṅkī,",
  "ja492:3.4_0": "asoko akutobhayo”.",
  "ja492:4.1_0": "“Aññampi leṇaṁ pariyesa,",
  "ja492:4.2_0": "sattu no idha vijjati;",
  "ja492:4.3_0": "So taccha sūkare hanti,",
  "ja492:4.4_0": "idhāgantvā varaṁ varaṁ”.",
  "ja492:5.1_0": "“Ko numhākaṁ idha sattu,",
  "ja492:5.2_0": "ko ñātī susamāgate;",
  "ja492:5.3_0": "Duppadhaṁse padhaṁseti,",
  "ja492:5.4_0": "taṁ me akkhātha pucchitā”.",
  "ja492:6.1_0": "“Uddhaggarājī migarājā,",
  "ja492:6.2_0": "balī dāṭhāvudho migo;",
  "ja492:6.3_0": "So taccha sūkare hanti,",
  "ja492:6.4_0": "idhāgantvā varaṁ varaṁ”.",
  "ja492:7.1_0": "“Na no dāṭhā na vijjanti,",
  "ja492:7.2_0": "balaṁ kāye samohitaṁ;",
  "ja492:7.3_0": "Sabbe samaggā hutvāna,",
  "ja492:7.4_0": "vasaṁ kāhāma ekakaṁ”.",
  "ja492:8.1_0": "“Hadayaṅgamaṁ kaṇṇasukhaṁ,",
  "ja492:8.2_0": "Vācaṁ bhāsasi tacchaka;",
  "ja492:8.3_0": "Yopi yuddhe palāyeyya,",
  "ja492:8.4_0": "Tampi pacchā hanāmase”.",
  "ja492:9.1_0": "“Pāṇātipātā virato nu ajja,",
  "ja492:9.2_0": "Abhayaṁ nu te sabbabhūtesu dinnaṁ;",
  "ja492:9.3_0": "Dāṭhā nu te migavadhāya na santi,",
  "ja492:9.4_0": "Yo saṅghapatto kapaṇova jhāyasi”.",
  "ja492:10.1_0": "“Na me dāṭhā na vijjanti,",
  "ja492:10.2_0": "Balaṁ kāye samohitaṁ;",
  "ja492:10.3_0": "Ñātī ca disvāna sāmaggī ekato,",
  "ja492:10.4_0": "Tasmā ca jhāyāmi vanamhi ekako.",
  "ja492:11.1_0": "Imassudaṁ yanti disodisaṁ pure,",
  "ja492:11.2_0": "Bhayaṭṭitā leṇagavesino puthu;",
  "ja492:11.3_0": "Te dāni saṅgamma vasanti ekato,",
  "ja492:11.4_0": "Yatthaṭṭhitā duppasahajja te mayā.",
  "ja492:12.1_0": "Pariṇāyakasampannā,",
  "ja492:12.2_0": "sahitā ekavādino;",
  "ja492:12.3_0": "Te maṁ samaggā hiṁseyyuṁ,",
  "ja492:12.4_0": "tasmā nesaṁ na patthaye”.",
  "ja492:13.1_0": "“Ekova indo asure jināti,",
  "ja492:13.2_0": "Ekova seno hanti dije pasayha;",
  "ja492:13.3_0": "Ekova byaggho migasaṅghapatto,",
  "ja492:13.4_0": "Varaṁ varaṁ hanti balañhi tādisaṁ”.",
  "ja492:14.1_0": "“Na heva indo na seno,",
  "ja492:14.2_0": "napi byaggho migādhipo;",
  "ja492:14.3_0": "Samagge sahite ñātī,",
  "ja492:14.4_0": "na byagghe kurute vase”.",
  "ja492:15.1_0": "“Kumbhīlakā sakuṇakā,",
  "ja492:15.2_0": "saṅghino gaṇacārino;",
  "ja492:15.3_0": "Sammodamānā ekajjhaṁ,",
  "ja492:15.4_0": "uppatanti ḍayanti ca.",
  "ja492:16.1_0": "Tesañca ḍayamānānaṁ,",
  "ja492:16.2_0": "ekettha apasakkati;",
  "ja492:16.3_0": "Tañca seno nitāḷeti,",
  "ja492:16.4_0": "veyyagghiyeva sā gati”.",
  "ja492:17.1_0": "“Ussāhito jaṭilena,",
  "ja492:17.2_0": "luddenāmisacakkhunā;",
  "ja492:17.3_0": "Dāṭhī dāṭhīsu pakkhandi,",
  "ja492:17.4_0": "maññamāno yathā pure”.",
  "ja492:18.1_0": "“Sādhu sambahulā ñātī,",
  "ja492:18.2_0": "api rukkhā araññajā;",
  "ja492:18.3_0": "Sūkarehi samaggehi,",
  "ja492:18.4_0": "byaggho ekāyane hato”.",
  "ja492:19.1_0": "Brāhmaṇañceva byagghañca,",
  "ja492:19.2_0": "ubho hantvāna sūkarā.",
  "ja492:19.3_0": "Ānandino pamuditā,",
  "ja492:19.4_0": "mahānādaṁ panādisuṁ.",
  "ja492:20.1_0": "Te su udumbaramūlasmiṁ,",
  "ja492:20.2_0": "sūkarā susamāgatā;",
  "ja492:20.3_0": "Tacchakaṁ abhisiñciṁsu,",
  "ja492:20.4_0": "“tvaṁ no rājāsi issaro”ti.",
  "ja492:21.1_0": "Tacchasūkarajātakaṁ navamaṁ."
}