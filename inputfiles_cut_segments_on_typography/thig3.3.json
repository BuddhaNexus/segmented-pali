{
  "thig3.3:0.1_0": "Therīgāthā 3.3",
  "thig3.3:0.2_0": "Tikanipāta",
  "thig3.3:0.3_0": "Aparāuttamātherīgāthā",
  "thig3.3:1.1_0": "“Ye ime satta bojjhaṅgā,",
  "thig3.3:1.2_0": "maggā nibbānapattiyā;",
  "thig3.3:1.3_0": "Bhāvitā te mayā sabbe,",
  "thig3.3:1.4_0": "yathā buddhena desitā.",
  "thig3.3:2.1_0": "Suññatassānimittassa,",
  "thig3.3:2.2_0": "lābhinīhaṁ yadicchakaṁ;",
  "thig3.3:2.3_0": "Orasā dhītā buddhassa,",
  "thig3.3:2.4_0": "nibbānābhiratā sadā.",
  "thig3.3:3.1_0": "Sabbe kāmā samucchinnā,",
  "thig3.3:3.2_0": "ye dibbā ye ca mānusā;",
  "thig3.3:3.3_0": "Vikkhīṇo jātisaṁsāro,",
  "thig3.3:3.4_0": "natthi dāni punabbhavo”ti.",
  "thig3.3:4.1_0": "… Aparā uttamā therī …."
}