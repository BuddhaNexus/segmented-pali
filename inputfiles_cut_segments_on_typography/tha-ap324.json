{
  "tha-ap324:0.1_0": "Therāpadāna",
  "tha-ap324:0.2_0": "Umāpupphiyavagga",
  "tha-ap324:0.3_0": "2 Pulinapūjakattheraapadāna",
  "tha-ap324:1.1_0": "“Kakudhaṁ vilasantaṁva,",
  "tha-ap324:1.2_0": "nisabhājāniyaṁ yathā;",
  "tha-ap324:1.3_0": "Osadhiṁva virocantaṁ,",
  "tha-ap324:1.4_0": "obhāsantaṁ narāsabhaṁ.",
  "tha-ap324:2.1_0": "Añjaliṁ paggahetvāna,",
  "tha-ap324:2.2_0": "avandiṁ satthuno ahaṁ;",
  "tha-ap324:2.3_0": "Satthāraṁ parivaṇṇesiṁ,",
  "tha-ap324:2.4_0": "sakakammena tosayiṁ.",
  "tha-ap324:3.1_0": "Susuddhaṁ pulinaṁ gayha,",
  "tha-ap324:3.2_0": "gatamagge samokiriṁ;",
  "tha-ap324:3.3_0": "Ucchaṅgena gahetvāna,",
  "tha-ap324:3.4_0": "vipassissa mahesino.",
  "tha-ap324:4.1_0": "Tato upaḍḍhapulinaṁ,",
  "tha-ap324:4.2_0": "vippasannena cetasā;",
  "tha-ap324:4.3_0": "Divāvihāre osiñciṁ,",
  "tha-ap324:4.4_0": "dvipadindassa tādino.",
  "tha-ap324:5.1_0": "Ekanavutito kappe,",
  "tha-ap324:5.2_0": "pulinaṁ yamasiñcahaṁ;",
  "tha-ap324:5.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap324:5.4_0": "pulinassa idaṁ phalaṁ.",
  "tha-ap324:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap324:6.2_0": "…pe…",
  "tha-ap324:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap324:7.1_0": "Itthaṁ sudaṁ āyasmā pulinapūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap324:8.1_0": "Pulinapūjakattherassāpadānaṁ dutiyaṁ."
}