{
  "anya-e0607n0:0_0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e0607n0:1_0": "Pajjamadhu",
  "anya-e0607n0:2_0": "Ānanda rañña ratanādi mahā yatinda,",
  "anya-e0607n0:3_0": "Niccappabuddha padumappiya sevinaṅgī;",
  "anya-e0607n0:4_0": "Buddhappiyena ghana buddha guṇappiyena,",
  "anya-e0607n0:5_0": "Therālinā racita pajjamadhuṃ pibantu.",
  "anya-e0607n0:6_0": "1.",
  "anya-e0607n0:7_0": "Uṇṇāpapuṇṇasasimaṇdalato galitvā,",
  "anya-e0607n0:8_0": "Pādambujaṅguli dalaṭṭha sudhā lavānaṃ;",
  "anya-e0607n0:9_0": "Pantīva satthu nakhapanti pajāvisesaṃ,",
  "anya-e0607n0:10_0": "Pīṇetu suddha sukhitammaṇa tunḍapītā.",
  "anya-e0607n0:11_0": "2.",
  "anya-e0607n0:12_0": "Khittāya māraripunā parivatya satthu,",
  "anya-e0607n0:13_0": "Pādassayā jita disāya sitattalāya;",
  "anya-e0607n0:14_0": "Yā jeti kañcana sarāvaliyā siriṃ sā,",
  "anya-e0607n0:15_0": "Detanginaṃ raṇajayaṅgulipantikantā.",
  "anya-e0607n0:16_0": "3.",
  "anya-e0607n0:17_0": "Sovaṇṇa vaṇṇa sukhumacchavi somma kumma,",
  "anya-e0607n0:18_0": "Piṭṭhīva piṭṭhi kamatunnati bhāti yesaṃ;",
  "anya-e0607n0:19_0": "Tesuppatiṭṭhitasukomaladīghapaṇhi,",
  "anya-e0607n0:20_0": "Pādā jinassa padadantu padaṃ janassa.",
  "anya-e0607n0:21_0": "4.",
  "anya-e0607n0:22_0": "Acchera paṅkajasiraṃ siriyā sakāya,",
  "anya-e0607n0:23_0": "Ye maddino viya caranti saroja sīse;",
  "anya-e0607n0:24_0": "Sañcumbitā viya ca tāni parāga rāgā,",
  "anya-e0607n0:25_0": "Te nīrajā munipadā padadantu lakkhiṃ.",
  "anya-e0607n0:26_0": "5.",
  "anya-e0607n0:27_0": "Agāmi kāla jana maṅgala bhattu bhāvaṃ,",
  "anya-e0607n0:28_0": "Vyākattumatra kusaleniva nimmitāni;",
  "anya-e0607n0:29_0": "Yātrāsumaṭṭhasatamaṅgalakkhaṇāni,",
  "anya-e0607n0:30_0": "Sādhetu naṃ padayugaṃ jayamaṅgalāni.",
  "anya-e0607n0:31_0": "6.",
  "anya-e0607n0:32_0": "Sassevijantuvarasantipurappavese,",
  "anya-e0607n0:33_0": "Niccaṃ susajja ṭhapitāniva maṅgalāya;",
  "anya-e0607n0:34_0": "Ye te dadhanti kalamaṅgalalakkhaṇāni,",
  "anya-e0607n0:35_0": "Vattantu te jinapadā jayamaṅgalāya.",
  "anya-e0607n0:36_0": "7.",
  "anya-e0607n0:37_0": "Sabbebhibhūya sapadesu nipātanassa,",
  "anya-e0607n0:38_0": "Saññāṇakaṃ viya yadassitasabbaloko;",
  "anya-e0607n0:39_0": "Pādātyadhokatatilokasirovarā pi,",
  "anya-e0607n0:40_0": "Lokaṃ puṇantu jayamaṅgalakāraṇāni.",
  "anya-e0607n0:41_0": "8.",
  "anya-e0607n0:42_0": "Lokattayekasaraṇattavibhāvanāya,",
  "anya-e0607n0:43_0": "Sajjo va tiṭṭhati yahiṃ suvibhattaloko;",
  "anya-e0607n0:44_0": "Taṃ sabbalokapaṭibimbitadappaṇābhaṃ,",
  "anya-e0607n0:45_0": "Pādadvayaṃ janasusajjanahetu hotu.",
  "anya-e0607n0:46_0": "9.",
  "anya-e0607n0:47_0": "Lokuttarāya siriyādhigamāya suṭṭhu,",
  "anya-e0607n0:48_0": "Rajanti yattha diguṇāniva pātu bhūtā;",
  "anya-e0607n0:49_0": "Cakkāsanābhisahanemisahassarāni,",
  "anya-e0607n0:50_0": "Tyaṅghī disantu sakalissariyaṃ janassa.",
  "anya-e0607n0:51_0": "10.",
  "anya-e0607n0:52_0": "Yatrullasanti duvidhāniva pātū bhūtā,",
  "anya-e0607n0:53_0": "Dhammassasabbabhuvanassa ca issaratte;",
  "anya-e0607n0:54_0": "Cakkāni cakkasadisāni sudassanassa,",
  "anya-e0607n0:55_0": "Tānajja jantu saraṇā caraṇāni hontu.",
  "anya-e0607n0:56_0": "11.",
  "anya-e0607n0:57_0": "Sattesu vacchatu sirī sirivacchakena,",
  "anya-e0607n0:58_0": "Sovatthi sotthimanutiṭṭhatu puggalesu;",
  "anya-e0607n0:59_0": "Nandiṃ janānamanuvattatu nandivattī,",
  "anya-e0607n0:60_0": "Sīsānalaṃkurutu pādavataṃsako pi.",
  "anya-e0607n0:61_0": "12.",
  "anya-e0607n0:62_0": "Bhaddāya pīṭhamupagacchatu bhaddapīṭhaṃ,",
  "anya-e0607n0:63_0": "Vuddhiṃ janānamanuvattatu vaddhamānaṃ;",
  "anya-e0607n0:64_0": "Puṇṇattamaṅgimanukubbatu puṇṇakumbho,",
  "anya-e0607n0:65_0": "Pāti ca pātu satataṃ janataṃ apāyā.",
  "anya-e0607n0:66_0": "13.",
  "anya-e0607n0:67_0": "Setātapattamapanetamaghātape taṃ,",
  "anya-e0607n0:68_0": "Khaggo vichindatu sadā duritārivagge;",
  "anya-e0607n0:69_0": "Saṃklesadāhamapanetu satālavaṇṭa,",
  "anya-e0607n0:70_0": "Saṃvījanī kumatimakkhikamorahattho.",
  "anya-e0607n0:71_0": "14.",
  "anya-e0607n0:72_0": "Ākaḍḍhano janavilocanamattaninnaṃ,",
  "anya-e0607n0:73_0": "Vāretu sabbagativāranamaṅkuso so;",
  "anya-e0607n0:74_0": "Pādambujassirivilāsaniketanaṃ va,",
  "anya-e0607n0:75_0": "Pāsādalakhaṇamupetu manopasādaṃ.",
  "anya-e0607n0:76_0": "15.",
  "anya-e0607n0:77_0": "Pāṇīnamattabhajataṃ varapuṇṇapattaṃ,",
  "anya-e0607n0:78_0": "Sammā dadātu padanissitapuṇṇapatto;",
  "anya-e0607n0:79_0": "Pādesu jantu manabandhanadāmabhūtaṃ,",
  "anya-e0607n0:80_0": "Dāmaṃ dametu vimalaṃ janatammanāni.",
  "anya-e0607n0:81_0": "16.",
  "anya-e0607n0:82_0": "Uṇhīsakuppalamaṇīpadumehi pādā,",
  "anya-e0607n0:83_0": "Sassevijantukaraṇāni vibhūsayantu;",
  "anya-e0607n0:84_0": "Sannettanāvupagatānamanagghakāni,",
  "anya-e0607n0:85_0": "Bojjhaṅgasattaratanāni dade samuddo.",
  "anya-e0607n0:86_0": "17.",
  "anya-e0607n0:87_0": "Uttuṅga niccalaguṅā jitatāya niccaṃ,",
  "anya-e0607n0:88_0": "Sevīva pādasiri nicca samubbahaṃ va;",
  "anya-e0607n0:89_0": "Atrāpi sakkabhavanubbahaṇe niyutto,",
  "anya-e0607n0:90_0": "Pādaṭṭhameru bhavataṃ bhavataṃ vibhūtyā.",
  "anya-e0607n0:91_0": "18.",
  "anya-e0607n0:92_0": "So cakkavāḷasikharī pyavataṃ samantā,",
  "anya-e0607n0:93_0": "Sabbūpasaggavisarājanataṃ samaggaṃ;",
  "anya-e0607n0:94_0": "Dīpā puthūpi caturo dvisahassa khuddā,",
  "anya-e0607n0:95_0": "Dhārentvapāyapatamānamadatva jantuṃ.",
  "anya-e0607n0:96_0": "19.",
  "anya-e0607n0:97_0": "Sūro pabodhayatu jantu saroruhāni,",
  "anya-e0607n0:98_0": "Cando pasāda kumudāni manodahesu;",
  "anya-e0607n0:99_0": "Nakkhattajātamakhilaṃ subhatāya hotu,",
  "anya-e0607n0:100_0": "Cakkaṃ dhajaṃ ripujayāya jayaddhajāya.",
  "anya-e0607n0:101_0": "20.",
  "anya-e0607n0:102_0": "Jetuṃ sasaṃsada-sudassana-cakkavatti,",
  "anya-e0607n0:103_0": "Cakkānugantalalitaṃ yahimāvaheyya;",
  "anya-e0607n0:104_0": "Cakkāṇuvatti parisāvata-cakkavatti,",
  "anya-e0607n0:105_0": "Naṃvattataṃ padayugaṃ janatā hitāya.",
  "anya-e0607n0:106_0": "21.",
  "anya-e0607n0:107_0": "Pujetumāgata vatā vajirāsanaṭṭha,",
  "anya-e0607n0:108_0": "Mindena chaḍḍita mahāvijayuttarākhyaṃ;",
  "anya-e0607n0:109_0": "Saṃkhaṃ paviṭṭhamiva mārabhayā padādho,",
  "anya-e0607n0:110_0": "Pādaṭṭhasaṃkhamiha vattatu santiyā vo.",
  "anya-e0607n0:111_0": "22.",
  "anya-e0607n0:112_0": "Sovaṇṇamacchayugalaṃ sivabhatta bhoge,",
  "anya-e0607n0:113_0": "Icchā bahūpakaraṇaṃ bhavataṃ janānaṃ;",
  "anya-e0607n0:114_0": "Kumbhīladhiggahitato va padutthacittā,",
  "anya-e0607n0:115_0": "Pādambujākara vigāhi tu nopahontu.",
  "anya-e0607n0:116_0": "23.",
  "anya-e0607n0:117_0": "Sattāpagā janamanoja male jahantu,",
  "anya-e0607n0:118_0": "Saṃklesadāhamapanentu dahā ca satta;",
  "anya-e0607n0:119_0": "Selā ca satta vidadhantu janassa tānaṃ,",
  "anya-e0607n0:120_0": "Lokappasiddhijanane bhavataṃ patākā.",
  "anya-e0607n0:121_0": "24.",
  "anya-e0607n0:122_0": "Pāṭaṅki santi gamane bhavatūpakārā,",
  "anya-e0607n0:123_0": "Dāhettanesu jahataṃ padacāmaraṃ taṃ;",
  "anya-e0607n0:124_0": "Sallokalocanamahussava-ussitaṃ va,",
  "anya-e0607n0:125_0": "Vatteyya toraṇamanuttaramaṅgalāya.",
  "anya-e0607n0:126_0": "25.",
  "anya-e0607n0:127_0": "Yasmiṃ miginda gata bhīti balāva daḍḍha,",
  "anya-e0607n0:128_0": "Dānā natā siravidāraṇa pīḷitāva;",
  "anya-e0607n0:129_0": "Nālāgirī karivaro girimekhalo ca,",
  "anya-e0607n0:130_0": "Taṃ sīhavikkamapadaṃ hanatā ghadantiṃ.",
  "anya-e0607n0:131_0": "26.",
  "anya-e0607n0:132_0": "Pāpāhino hanatu pādasuvaṇṇarājā,",
  "anya-e0607n0:133_0": "Vyagghādhipo kalijane adataṃ asesaṃ;",
  "anya-e0607n0:134_0": "Vālāha-assapati sampatituṃ adatvā,",
  "anya-e0607n0:135_0": "Pāyesu pāpayatu santipurampajāyo.",
  "anya-e0607n0:136_0": "27.",
  "anya-e0607n0:137_0": "Chaddanta danti lalitaṃ galitaṃ rusamhā,",
  "anya-e0607n0:138_0": "Luddetta dubbhini dise acalaṃ dadhāno;",
  "anya-e0607n0:139_0": "Pādaṭṭhahatthipati sampati jantutāse,",
  "anya-e0607n0:140_0": "Tāsetu hāsamaparandisataṃ satānaṃ.",
  "anya-e0607n0:141_0": "28.",
  "anya-e0607n0:142_0": "Sabbaṅgino caraṇuposatha hatthirājā,",
  "anya-e0607n0:143_0": "Pāpetu sabbacatudīpikarajjalakkhiṃ;",
  "anya-e0607n0:144_0": "Kittīva pādaparicārikatā niyuttā,",
  "anya-e0607n0:145_0": "Kelasaselapaṭimā hitamācareyya.",
  "anya-e0607n0:146_0": "29.",
  "anya-e0607n0:147_0": "Sāmissa haṃsasamaye dahapāsabaddha,",
  "anya-e0607n0:148_0": "Māsīna vesagamako viya pādahaṃso;",
  "anya-e0607n0:149_0": "Nigghosa gantijitato viya mūgapakkho,",
  "anya-e0607n0:150_0": "Yāretu sabba janatā bhavagantukattaṃ.",
  "anya-e0607n0:151_0": "30.",
  "anya-e0607n0:152_0": "Ohāya dibbasarasiṃ khilaloka sabba,",
  "anya-e0607n0:153_0": "Rammaṅghivāpimavagāhitavāva pāde;",
  "anya-e0607n0:154_0": "Erāvaṇo karivaro manasābhiruḷhe,",
  "anya-e0607n0:155_0": "Jantuṃ purindadapuraṃ nayataṃ va sīghaṃ.",
  "anya-e0607n0:156_0": "31.",
  "anya-e0607n0:157_0": "Hitvā sakambhavanamaṅghinisevanattha,",
  "anya-e0607n0:158_0": "Māgamma ramma taratāyiha nissito va;",
  "anya-e0607n0:159_0": "Pāletva mūni padavāpitaraṅgabhaṅgi,",
  "anya-e0607n0:160_0": "Mangī karontatanuvāsuki nāgarājā.",
  "anya-e0607n0:161_0": "32.",
  "anya-e0607n0:162_0": "Nāthassa kañcanasikhāvalajātilīla,",
  "anya-e0607n0:163_0": "Māvikaraṃ va padanissitamorarājā;",
  "anya-e0607n0:164_0": "Taṃ dhammadesanaraveniva luddakassa,",
  "anya-e0607n0:165_0": "Lokassa pāpaphaṇino hanataṃ asesaṃ.",
  "anya-e0607n0:166_0": "33.",
  "anya-e0607n0:167_0": "Saṃsārasāgaragate sadhane jane te,",
  "anya-e0607n0:168_0": "Netampade kalacatummukhahemanāvā;",
  "anya-e0607n0:169_0": "Nibbāṇapaṭṭanavaraṃ bharukacchakantaṃ,",
  "anya-e0607n0:170_0": "Suppārapaṇḍita gatā viya āsunāvā.",
  "anya-e0607n0:171_0": "34.",
  "anya-e0607n0:172_0": "Sambodhi ñāṇa paripācayato munissa,",
  "anya-e0607n0:173_0": "Bhatto yathā himavataddi samādhihetu;",
  "anya-e0607n0:174_0": "Evammanena bhajataṃ himavaddipāde,",
  "anya-e0607n0:175_0": "Sambodhiñāṇa paripācanahetu hotu.",
  "anya-e0607n0:176_0": "35.",
  "anya-e0607n0:177_0": "Daḷhaṃ parājitatayā muninā sarena,",
  "anya-e0607n0:178_0": "Suññassaropagata pañjara bandhanova;",
  "anya-e0607n0:179_0": "So pādapañjaragato karavīkapakkhī,",
  "anya-e0607n0:180_0": "Sabbesamappīyāvacañjahatā bhavantaṃ.",
  "anya-e0607n0:181_0": "36.",
  "anya-e0607n0:182_0": "Te cakkavāka makarā api koñca jīvaṃ,",
  "anya-e0607n0:183_0": "Jīvādi pakkhivisarā sarasīva bhuttaṃ;",
  "anya-e0607n0:184_0": "Vessantarena caraṇambuji nibbhajantā,",
  "anya-e0607n0:185_0": "Jantu tahiṃ viya pade suramentu niccaṃ.",
  "anya-e0607n0:186_0": "37.",
  "anya-e0607n0:187_0": "Taṃ candakinnaragatiṃva gatassa bodhi,",
  "anya-e0607n0:188_0": "Sattassa tassa sapajāpatikassa bhāvaṃ;",
  "anya-e0607n0:189_0": "Saṃsūcayanta pada kinnara kinnarī ve,",
  "anya-e0607n0:190_0": "Sāmaggimagga paṭi pattisu pāpayantu.",
  "anya-e0607n0:191_0": "38.",
  "anya-e0607n0:192_0": "Saṃrājadhānimusabho vahatagga bhāraṃ,",
  "anya-e0607n0:193_0": "Pītippayo pajanayeyya savacchadhenu;",
  "anya-e0607n0:194_0": "Sassevino abhiramentu chakāmasaggā,",
  "anya-e0607n0:195_0": "Dhārentu jhāyimiha soḷasa dhātudhāmā.",
  "anya-e0607n0:196_0": "39.",
  "anya-e0607n0:197_0": "Sutvā jinassa karavīka sarammanuññaṃ,",
  "anya-e0607n0:198_0": "Aññoñña bhītirahitā api paccanīkā;",
  "anya-e0607n0:199_0": "Hitvā gatiṃ viya ṭhitā padasattarūpā,",
  "anya-e0607n0:200_0": "Sabbaṃ bhavassita janānagatiṃ hanantu.",
  "anya-e0607n0:201_0": "40.",
  "anya-e0607n0:202_0": "Sovaṇṇa kāhaḷa yugo pamamindirāya,",
  "anya-e0607n0:203_0": "Sannīrapuppha mukulopamamussavāya;",
  "anya-e0607n0:204_0": "Niccaṃ susajja ṭhapitaṃ muni tiṭṭhatante,",
  "anya-e0607n0:205_0": "Jaṅghādvayaṃ janavilocana maṅgalāya.",
  "anya-e0607n0:206_0": "41.",
  "anya-e0607n0:207_0": "Lakhyā vilāsa mukuradvaya sannikāsaṃ,",
  "anya-e0607n0:208_0": "Tāḍaṅka maṇḍana viḍambakamaṃsu saṇḍaṃ;",
  "anya-e0607n0:209_0": "Jānudvayaṃ laḷita sāgara bubbalābhaṃ,",
  "anya-e0607n0:210_0": "Hotaṃ jagattaya nijatta vibhūsitunte.",
  "anya-e0607n0:211_0": "42.",
  "anya-e0607n0:212_0": "Chaddanti dinna varadanta yugopamānā,",
  "anya-e0607n0:213_0": "Taṃ hatthi soṇḍa kama puṇṇa guṇā tavorū;",
  "anya-e0607n0:214_0": "Līla payodhi siri keḷi suvaṇṇarambhā,",
  "anya-e0607n0:215_0": "Khandhāva dentu paripuṇṇa gune janānāṃ.",
  "anya-e0607n0:216_0": "43.",
  "anya-e0607n0:217_0": "Jaṅghakkha kadvya samappita cittapāda,",
  "anya-e0607n0:218_0": "Cakkadvayī manamanojahayo mune te;",
  "anya-e0607n0:219_0": "Sonī ratho sirivaho manasā bhiruḷhaṃ,",
  "anya-e0607n0:220_0": "Lokattayaṃ sivapuraṃ lahu pāpayātu.",
  "anya-e0607n0:221_0": "44.",
  "anya-e0607n0:222_0": "Rammora pākaṭa taṭāka taṭā savanta,",
  "anya-e0607n0:223_0": "Romāvalī jala panālika koṭikaṭṭhā;",
  "anya-e0607n0:224_0": "Nābhī gabhīra sarasī siri keḷitā te,",
  "anya-e0607n0:225_0": "Sassevinaṃ vyasana ghammamalaṃ sametu.",
  "anya-e0607n0:226_0": "45.",
  "anya-e0607n0:227_0": "Kanticchaṭā luḷita rūpa payodhi nābhi,",
  "anya-e0607n0:228_0": "Āvaṭṭa vaṭṭita nimujjita sabbaloko;",
  "anya-e0607n0:229_0": "Sobhagga toya nivahaṃ visaso pivitvā,",
  "anya-e0607n0:230_0": "Lokuttarādi sukha mucchitataṃ payātu.",
  "anya-e0607n0:231_0": "46.",
  "anya-e0607n0:232_0": "Gambhīra cittarahadaṃ paripūrayitvā,",
  "anya-e0607n0:233_0": "Taṃ sandamāna karuṇambu pavāha tulyā;",
  "anya-e0607n0:234_0": "Romālivallihari nābhi subhālavālā,",
  "anya-e0607n0:235_0": "Detaṃ lahuṃ sivaphalaṃ bhajataṃ mune te.",
  "anya-e0607n0:236_0": "47.",
  "anya-e0607n0:237_0": "Cārūra sāriphalako kuṭilagga loma,",
  "anya-e0607n0:238_0": "Pantī vibhatti sahito siri keḷi sajjo;",
  "anya-e0607n0:239_0": "Saggāpavagga sukha jūtaka keḷi hetu,",
  "anya-e0607n0:240_0": "Hotaṃ tiloka sukha jūtaka soṇḍakānaṃ.",
  "anya-e0607n0:241_0": "48.",
  "anya-e0607n0:242_0": "Gambhīra citta rahado dara gāhamāna,",
  "anya-e0607n0:243_0": "Mettādayā kari vadhū kara sanni kāsā;",
  "anya-e0607n0:244_0": "Sabbaṅginaṃ sivaphalaṃ tanu deva rukkhe,",
  "anya-e0607n0:245_0": "Sākhā sakhā tava bhujā bhajataṃ dadantu.",
  "anya-e0607n0:246_0": "49.",
  "anya-e0607n0:247_0": "Nihāra bindu sahitaggadalopa sobhi,",
  "anya-e0607n0:248_0": "Byālamba ratta padumadvaya bhaṅgi bhājā;",
  "anya-e0607n0:249_0": "Pāpārisīsalunateniva ratta rattā,",
  "anya-e0607n0:250_0": "Rattā karā tava bhavumbhuvi maṅgalāya.",
  "anya-e0607n0:251_0": "50.",
  "anya-e0607n0:252_0": "Rupassirī carita caṅkama vibbhamā te,",
  "anya-e0607n0:253_0": "Piṭṭhī yathā kalala muddhani setu bhūtā;",
  "anya-e0607n0:254_0": "Evaṃ bhavaṇṇava samuttaraṇāya setu,",
  "anya-e0607n0:255_0": "Hotammahākanaka saṃkama sannikāsā.",
  "anya-e0607n0:256_0": "51.",
  "anya-e0607n0:257_0": "Saddhamma desana manohara bherināda,",
  "anya-e0607n0:258_0": "Saṃcāraṇe sivapuraṃ visituṃ janānaṃ;",
  "anya-e0607n0:259_0": "Gīvā suvaṇṇamaya cāru mutiṅga bheri,",
  "anya-e0607n0:260_0": "Bhāvambhajā bhavatu bhūta vibhūtiyā te.",
  "anya-e0607n0:261_0": "52.",
  "anya-e0607n0:262_0": "Lakhī nivāsa vadanambuja matta ninna,",
  "anya-e0607n0:263_0": "Mākaḍḍhayaṃ jana vilocana cañcarīke;",
  "anya-e0607n0:264_0": "Sorabbha dhamma makaranda nisandamānaṃ,",
  "anya-e0607n0:265_0": "Piṇetu tena sarasena sabhā jane te.",
  "anya-e0607n0:266_0": "53.",
  "anya-e0607n0:267_0": "Lakhī samāruhita vattarathe rathaṅga,",
  "anya-e0607n0:268_0": "Dvandānu kāri miga rāja kapola līlaṃ;",
  "anya-e0607n0:269_0": "Tādaṅka maṇḍalayugaṃ viya kaṇṇabhājaṃ,",
  "anya-e0607n0:270_0": "Gaṇḍatthaladvyamalaṃkurutaṃ janatte.",
  "anya-e0607n0:271_0": "54.",
  "anya-e0607n0:272_0": "Lāvaṇṇa maṇṇava pavāḷa latā dvayābhaṃ,",
  "anya-e0607n0:273_0": "Tandeha deva taru pallava kante mantaṃ;",
  "anya-e0607n0:274_0": "Vattāravinda makaranda parājisobhaṃ,",
  "anya-e0607n0:275_0": "Rattādharadvayamadho kurutaṃ janāghaṃ.",
  "anya-e0607n0:276_0": "55.",
  "anya-e0607n0:277_0": "Uṇṇā sakuntigata matthaka natthu kūpa,",
  "anya-e0607n0:278_0": "Subbhū lakāra sahitoṭṭha pavāḷa nāvā;",
  "anya-e0607n0:279_0": "Gattuttararaṇṇava gatā tava jantukānaṃ,",
  "anya-e0607n0:280_0": "Hotaṃ bhavaṇṇava samuttaranaya nātha.",
  "anya-e0607n0:281_0": "56.",
  "anya-e0607n0:282_0": "Isaṃ vikāsa padumodara kesarāli,",
  "anya-e0607n0:283_0": "Līlā vinaddha rucirā tava danta panti;",
  "anya-e0607n0:284_0": "Vānī vadhū dharita mālati mālya tulyā,",
  "anya-e0607n0:285_0": "Tassaṃ jānassa manarañjana mācareyya.",
  "anya-e0607n0:286_0": "57.",
  "anya-e0607n0:287_0": "Saddhamma nijjhara suratta silātalābhā,",
  "anya-e0607n0:288_0": "Jivhā vacī naṭa vadhū kala raṅga bhūtā;",
  "anya-e0607n0:289_0": "Saddhamma seṭṭha taraṇī nihitappiyā te,",
  "anya-e0607n0:290_0": "Saṃsāra sāgara samuttaraṇāya hotu.",
  "anya-e0607n0:291_0": "58.",
  "anya-e0607n0:292_0": "Dantaṃsu kañcukīta rattadharo padhāne,",
  "anya-e0607n0:293_0": "Jivhā suratta sayane mukha mandiraṭṭhe;",
  "anya-e0607n0:294_0": "Āmokkha mutti vadhuyā sayitāya tuyhaṃ,",
  "anya-e0607n0:295_0": "Kubbantu saṃgama malaṃ jana sotu kāmi.",
  "anya-e0607n0:296_0": "59.",
  "anya-e0607n0:297_0": "Uṇṇā tathābhinava patta varābhi rāmā,",
  "anya-e0607n0:298_0": "Līlollasanta bhamukadvaya nīla pattā;",
  "anya-e0607n0:299_0": "Ghānoru cāru kadalī vadanā lavālā,",
  "anya-e0607n0:300_0": "Tuyhaṃ pavattatu ciraṃ jana maṅgalāya.",
  "anya-e0607n0:301_0": "60.",
  "anya-e0607n0:302_0": "Bālatthalī hari silātala piṭṭhikaṭṭha,",
  "anya-e0607n0:303_0": "Bhūvallaridvaya mayūra yugassa tuyhaṃ;",
  "anya-e0607n0:304_0": "Pañcappabhā rucira piccha yugassirīkaṃ,",
  "anya-e0607n0:305_0": "Nettadvayaṃ manasi puñchatu pāpadhūliṃ.",
  "anya-e0607n0:306_0": "61.",
  "anya-e0607n0:307_0": "Indīvarāntagata bhiṅgika panti bhaṅgi,",
  "anya-e0607n0:308_0": "Pañambujassaratate viya gacchapantī;",
  "anya-e0607n0:309_0": "Nettambujassiri tirokaraṇīva tuyhaṃ,",
  "anya-e0607n0:310_0": "Pamhāvalī sirigateha tiro karontu.",
  "anya-e0607n0:311_0": "62.",
  "anya-e0607n0:312_0": "Vattullasambuja vilocana haṃsa tuṇḍa,",
  "anya-e0607n0:313_0": "Kañjaṃsu piñjara mulāla latā dvyābhaṃ;",
  "anya-e0607n0:314_0": "Dolādvayaṃva savaṇadvyamatta lakkhyā,",
  "anya-e0607n0:315_0": "Hotaṃ tavajja janatā maticārahetu.",
  "anya-e0607n0:316_0": "63.",
  "anya-e0607n0:317_0": "Vammīka matthaka sayānaka bhūridatta,",
  "anya-e0607n0:318_0": "Bhoginda bhogavali vibbhamamā vahanti;",
  "anya-e0607n0:319_0": "Ghānopariṭṭhitamune tava tuṇṇamuṇṇā,",
  "anya-e0607n0:320_0": "Taggāhino viya janassa dadātu vittaṃ.",
  "anya-e0607n0:321_0": "64.",
  "anya-e0607n0:322_0": "Rūpindirāya vijaye khila loka rūpaṃ,",
  "anya-e0607n0:323_0": "Ghāṇoru cāru parighopari baddha siddhā;",
  "anya-e0607n0:324_0": "Nīlābha vāta viluthanta vayaddhajābhā,",
  "anya-e0607n0:325_0": "Tiṭṭhantu sajja duritāri jayāya te bhū.",
  "anya-e0607n0:326_0": "65.",
  "anya-e0607n0:327_0": "Uṇṇassitopala nivesita bunda sandhi,",
  "anya-e0607n0:328_0": "Ghāṇoru piṇḍakamaghā tapa rundhitunte;",
  "anya-e0607n0:329_0": "Hotammukhambuja sirī sirasussitābhaṃ,",
  "anya-e0607n0:330_0": "Bhū nīla paṭṭika lalāta suvaṇṇa chattaṃ.",
  "anya-e0607n0:331_0": "66.",
  "anya-e0607n0:332_0": "Rupaṅka vedana vilocana bāna diṭṭhī,",
  "anya-e0607n0:333_0": "Dhārā nisāna maṇivaṭṭa sirī siro te;",
  "anya-e0607n0:334_0": "Siddhā matosadha katañjana puñja lakkhī,",
  "anya-e0607n0:335_0": "Hotaṃ janassa nayanāmaya nāsanāya.",
  "anya-e0607n0:336_0": "67.",
  "anya-e0607n0:337_0": "Sakkhandha bāhuyuga torana majjha gīvā,",
  "anya-e0607n0:338_0": "Dharappitassirighato pari mussavāya;",
  "anya-e0607n0:339_0": "Nīluppalāva ṭhapitā savibhatti kante,",
  "anya-e0607n0:340_0": "Kesā bhavantu bhuvanattaya maṅgalāya.",
  "anya-e0607n0:341_0": "68.",
  "anya-e0607n0:342_0": "Hemagghiye ṭhapita nīla silā kapāle,",
  "anya-e0607n0:343_0": "Pajjota jāla lalitaṃ muni sārayantī;",
  "anya-e0607n0:344_0": "Rūpassirī sirasi bhūsita hema mālā,",
  "anya-e0607n0:345_0": "Kārā karotu subhagaṃ tava ketu mālā.",
  "anya-e0607n0:346_0": "69.",
  "anya-e0607n0:347_0": "Bhyāmappabhāli tava kañcana mora kāle,",
  "anya-e0607n0:348_0": "Surodaye vitata candaka cakkalakkhī;",
  "anya-e0607n0:349_0": "Meghāvanaddha sikharunnata hema selā,",
  "anya-e0607n0:350_0": "Yantindacāpa vikatīva dadātu sobhaṃ.",
  "anya-e0607n0:351_0": "70.",
  "anya-e0607n0:352_0": "Paṭṭhāya te paṇidhito suci dāna sīla,",
  "anya-e0607n0:353_0": "Nekkhamma pañña viriyakkhama saccadhiṭṭhā;",
  "anya-e0607n0:354_0": "Mettā upekkhiti ime dasa pūratova,",
  "anya-e0607n0:355_0": "Pūrentu pārami guṇā janatānamatte.",
  "anya-e0607n0:356_0": "71.",
  "anya-e0607n0:357_0": "Pattuttaruttaradasā paṇidhāna bījā,",
  "anya-e0607n0:358_0": "Cetoradharāya karuṇā jala sekha vuddhā;",
  "anya-e0607n0:359_0": "Sabbaññu ñāṇa phaladā sati vāṭa guttā,",
  "anya-e0607n0:360_0": "Taṃ samphalandisatu pāramitā latā te.",
  "anya-e0607n0:361_0": "72.",
  "anya-e0607n0:362_0": "Ābodhi puṇṇami padiṭṭha dinādito te,",
  "anya-e0607n0:363_0": "Sambhāra kāla sita pakkha kamābhi vuddho;",
  "anya-e0607n0:364_0": "Sampuṇṇa pārami guṇāmataraṃsi taṃva,",
  "anya-e0607n0:365_0": "Sabbaṅgi kunda kumudāni pabodhayeyya.",
  "anya-e0607n0:366_0": "73.",
  "anya-e0607n0:367_0": "Āpacchimabbhava sivapphala lābha dānā,",
  "anya-e0607n0:368_0": "Dānappabandhamapidāna phalappabhandaṃ;",
  "anya-e0607n0:369_0": "Saṃvaḍḍhayi tvaṃ abhipatthanato yathevaṃ,",
  "anya-e0607n0:370_0": "Jantuttaruttara phalaṃ khalu sambhunantu.",
  "anya-e0607n0:371_0": "74.",
  "anya-e0607n0:372_0": "Ārambhatoppabhuti yāva tavaggamaggā,",
  "anya-e0607n0:373_0": "Vikkhālita ghakalusaṃ suci sīla toyaṃ;",
  "anya-e0607n0:374_0": "Mettā dayā madhura sītalatāyupetaṃ,",
  "anya-e0607n0:375_0": "Sodhetu tvaṃva bhava nissita jantu metaṃ.",
  "anya-e0607n0:376_0": "75.",
  "anya-e0607n0:377_0": "Āpaccimattamabhinikkhamanābhiyogā,",
  "anya-e0607n0:378_0": "Paṭṭhāya tampabhavato paripuṇṇa gehā;",
  "anya-e0607n0:379_0": "Tvaṃ sabba jāti gahato api nikkhamittho,",
  "anya-e0607n0:380_0": "Evaṃ janā bhava dukhā khalu nikkhamantu.",
  "anya-e0607n0:381_0": "76.",
  "anya-e0607n0:382_0": "Ekaggato pala tale nisitā cirandhi,",
  "anya-e0607n0:383_0": "Dhārā sucittu sutale sati daṇḍa baddhe;",
  "anya-e0607n0:384_0": "Nibbijjhi lakkhaṇa dhanuṭṭhiti santi lakkhaṃ,",
  "anya-e0607n0:385_0": "Khittā tayonamanu vijjhatu jantu khittā.",
  "anya-e0607n0:386_0": "77.",
  "anya-e0607n0:387_0": "Tvaṃ pāramī jala nidhiṃ caturiha bāhu,",
  "anya-e0607n0:388_0": "Sattīhi suttari ciraṃ janakova sindhuṃ;",
  "anya-e0607n0:389_0": "Sampanna vikkama phalosi yathā casova,",
  "anya-e0607n0:390_0": "Evaṃ janā viriyatapphalame dhayantu.",
  "anya-e0607n0:391_0": "78.",
  "anya-e0607n0:392_0": "Satta paradha dahanesu ciraṃ sudhantaṃ,",
  "anya-e0607n0:393_0": "Khantī suvaṇṇa kata rūpa samantimattā;",
  "anya-e0607n0:394_0": "Sabbā parādhamasahi tvaṃasayhamevaṃ,",
  "anya-e0607n0:395_0": "Sabbe janāpi khamanena bhajantu santiṃ.",
  "anya-e0607n0:396_0": "79.",
  "anya-e0607n0:397_0": "Lakkhādhikaṃ catura saṃkhiya kappa kālaṃ,",
  "anya-e0607n0:398_0": "Saccena suṭṭhu paribhāvita vācino te;",
  "anya-e0607n0:399_0": "Vācāya sacca phusitāya samenti jantu,",
  "anya-e0607n0:400_0": "Evaṃ visuddha vacanā janatā bhavantu.",
  "anya-e0607n0:401_0": "80.",
  "anya-e0607n0:402_0": "Ādinna dhamma mahiyatthira suppatiṭṭhā,",
  "anya-e0607n0:403_0": "Dhiṭṭhāna pārami mahā vajiraddi tuyhaṃ;",
  "anya-e0607n0:404_0": "Sattena kena pi yathāhi abhejja nejjo,",
  "anya-e0607n0:405_0": "Evaṃ janāpi kusalesu adhiṭṭha hantu.",
  "anya-e0607n0:406_0": "81.",
  "anya-e0607n0:407_0": "Tvaṃ sabba satta cirabhāvita metta citta,",
  "anya-e0607n0:408_0": "Toyehi saṃsamita kodha mahā hutāso;",
  "anya-e0607n0:409_0": "Lokuttaraṃ taditaraṃ hitamāvahittho,",
  "anya-e0607n0:410_0": "Evaṃ janesu janatā hitamāvahantū.",
  "anya-e0607n0:411_0": "82.",
  "anya-e0607n0:412_0": "Mittopakāra paṭipakkha janāpakāre,",
  "anya-e0607n0:413_0": "Tvaṃ nibbikāra manaso cirabhāvanāya;",
  "anya-e0607n0:414_0": "Pattosilābha pabhutaṭṭusu nibbikāraṃ,",
  "anya-e0607n0:415_0": "Evaṃ janānunaya kopa nudā bhavantu.",
  "anya-e0607n0:416_0": "83.",
  "anya-e0607n0:417_0": "Sampanna hetu vibhavo tusite vimānaṃ,",
  "anya-e0607n0:418_0": "Yuttaṃ guṇehi navabhippadavī vimānaṃ;",
  "anya-e0607n0:419_0": "Tvaṃ vādhiparamidhirohiniyā tiloko,",
  "anya-e0607n0:420_0": "Ārohatu bhaya sukhaṃ padavī vimānaṃ.",
  "anya-e0607n0:421_0": "84.",
  "anya-e0607n0:422_0": "Tvaṃverahaṃsi samabujjhi yathāca sammā,",
  "anya-e0607n0:423_0": "Sampanna vijja caraṇo sugatosi hontu;",
  "anya-e0607n0:424_0": "Lokaṃ vido purisadammasusārathī si,",
  "anya-e0607n0:425_0": "Satthāsi bujjhi bhagavā si tatheva jantu.",
  "anya-e0607n0:426_0": "85.",
  "anya-e0607n0:427_0": "Saccitta bhū nidahitaṃ janatāya tuyhaṃ,",
  "anya-e0607n0:428_0": "Kalyāṇavaṇṇaratanaṇṇavajātibhinnaṃ;",
  "anya-e0607n0:429_0": "Dukkhaggi cora jalupaddutajāti gehe,",
  "anya-e0607n0:430_0": "Tassā sukhaṃ bhavatu jīvitumāpadāya.",
  "anya-e0607n0:431_0": "86.",
  "anya-e0607n0:432_0": "Vācā vicitta vara tantu gataṅgi kaṇṭhe,",
  "anya-e0607n0:433_0": "Svā mutta sagguṇa mahā ratanā valī te;",
  "anya-e0607n0:434_0": "Vevaṇṇi yattani bhavaṃ sakalampahāya,",
  "anya-e0607n0:435_0": "Hotañjanassa siri saṅgama maṅgalāya.",
  "anya-e0607n0:436_0": "87.",
  "anya-e0607n0:437_0": "Taṃ sagguṇatthava dahaṭṭha sutippanāli,",
  "anya-e0607n0:438_0": "Nissandamāna guṇanīra nipāna tinte;",
  "anya-e0607n0:439_0": "Khettetta saññini janā kata loma haṃsa,",
  "anya-e0607n0:440_0": "Bījankurī kusala sassa phalaṃ labhantu.",
  "anya-e0607n0:441_0": "88.",
  "anya-e0607n0:442_0": "Āpāyikappabhuti dukkha nidāgha kāla,",
  "anya-e0607n0:443_0": "Santāpitā nikhila loka mano kadambā;",
  "anya-e0607n0:444_0": "Taṃ vaṅṇa megha phusanā hasanaṅkurehi,",
  "anya-e0607n0:445_0": "Iddhā bhavantu mati vallari vellītā te.",
  "anya-e0607n0:446_0": "89.",
  "anya-e0607n0:447_0": "Hetuddasā phaladasā samavaṭṭhitaṃ taṃ,",
  "anya-e0607n0:448_0": "Sabbattha satta hitamāvahaṇena siddhaṃ;",
  "anya-e0607n0:449_0": "Cintāpathātiganubhāva vibhāvanante,",
  "anya-e0607n0:450_0": "Bhūtānamatthu caritabbhutamattha sidhyā.",
  "anya-e0607n0:451_0": "90.",
  "anya-e0607n0:452_0": "Aṅgārakāsumabhilaṅghiya dāna kāle,",
  "anya-e0607n0:453_0": "Bhattattano pada paṭicchaka paṅkajā ca;",
  "anya-e0607n0:454_0": "Yātakkhaṇe tava pade dhaṭamuṭṭhahitvā,",
  "anya-e0607n0:455_0": "Paṅkeruhāṃ siva madhuṃ sarataṃ dadantu.",
  "anya-e0607n0:456_0": "91.",
  "anya-e0607n0:457_0": "Saccena maccha pati vassita vassadhārā,",
  "anya-e0607n0:458_0": "Satte dayāya tava vassita vassadhārā;",
  "anya-e0607n0:459_0": "Gimhe janassa samayiṃsu yathā tathātā,",
  "anya-e0607n0:460_0": "Dhamambuvuṭṭhiva samentu kilesa dāhe.",
  "anya-e0607n0:461_0": "92.",
  "anya-e0607n0:462_0": "Chaddanta nāga patinā khamatā parādhaṃ,",
  "anya-e0607n0:463_0": "Chetvā kare ṭhapita dantavarāva luddaṃ;",
  "anya-e0607n0:464_0": "Loke hitāya ṭhapitā tava danta dhātu,",
  "anya-e0607n0:465_0": "Seṭṭhā janaṃ siva puraṃ lahu pāpayantu.",
  "anya-e0607n0:466_0": "93.",
  "anya-e0607n0:467_0": "Taṃ temiyākhya yatinossama mālakamhi,",
  "anya-e0607n0:468_0": "Okiṇṇa mutta kanakā vuja vippakiṇṇā;",
  "anya-e0607n0:469_0": "Kāruñña vārida cuto daka bindu bandhū,",
  "anya-e0607n0:470_0": "Dhātu samentu tava jantusu dukkhadāhe.",
  "anya-e0607n0:471_0": "94.",
  "anya-e0607n0:472_0": "Raṭṭhassa attha caraṇāya asammukhassa,",
  "anya-e0607n0:473_0": "Rāmena dinna tiṇa saṃkhata pādukāva;",
  "anya-e0607n0:474_0": "Bhuttā tayā ciramasammukha nāgatassa,",
  "anya-e0607n0:475_0": "Lokassa atthamanu tiṭṭhatu patta dhātu.",
  "anya-e0607n0:476_0": "95.",
  "anya-e0607n0:477_0": "Vutto janānamupadissa varāha raññā,",
  "anya-e0607n0:478_0": "Satthiṃ sahassa saradaṃ viya ñāya dhammo;",
  "anya-e0607n0:479_0": "Ādeyya heyyamupadissa tayā pavutto,",
  "anya-e0607n0:480_0": "Dhammo pavattatu ciraṃ janatā hitāya.",
  "anya-e0607n0:481_0": "96.",
  "anya-e0607n0:482_0": "Mārāri maddana hitādhigamaṃ karotā,",
  "anya-e0607n0:483_0": "Bhatto tayā vara mahā jaya bodhi rājā;",
  "anya-e0607n0:484_0": "Saggā pavavagga hitahetu janassa hantvā,",
  "anya-e0607n0:485_0": "Sabbantarāyamiha tiṭṭhatu suṭṭhu sajjo.",
  "anya-e0607n0:486_0": "97.",
  "anya-e0607n0:487_0": "Sāmoda vaṇṇa bhajanī guṇa mañjarīyaṃ,",
  "anya-e0607n0:488_0": "Cariyā latā vikasitā tava sapphalaṅgaṃ;",
  "anya-e0607n0:489_0": "Okiṇṇa citta madhupe rasa pīṇayanti,",
  "anya-e0607n0:490_0": "Sambhāvitā bhuvi pavattatu matthakehi.",
  "anya-e0607n0:491_0": "98.",
  "anya-e0607n0:492_0": "Sambuddha selavalayantara jānanavhā,",
  "anya-e0607n0:493_0": "Nottattato tipathagā yati sāgaraṭṭhā;",
  "anya-e0607n0:494_0": "Dhammā pagā suti vase tarite puṇanti,",
  "anya-e0607n0:495_0": "Sambhāra sassamiha vattatu pacayanti.",
  "anya-e0607n0:496_0": "99.",
  "anya-e0607n0:497_0": "Paññāṇa kūpa sita paggaha vāyu gāhī,",
  "anya-e0607n0:498_0": "Saddhā lakāra sahitā sati pota vāhā;",
  "anya-e0607n0:499_0": "Sampāpayātu bhava sāgara pāra tīra,",
  "anya-e0607n0:500_0": "Sappattanaṃ varadhane pati patti nāvā.",
  "anya-e0607n0:501_0": "100.",
  "anya-e0607n0:502_0": "Bojjhaṅga satta ratanākara dhamma khandha,",
  "anya-e0607n0:503_0": "Gambhīra nīra caya sāsana sāgaro saṃ;",
  "anya-e0607n0:504_0": "So sīlyananta tanu veṭitha ñāṇa mantha,",
  "anya-e0607n0:505_0": "Selena manthitavataṃ disatā mataṃ ve.",
  "anya-e0607n0:506_0": "101.",
  "anya-e0607n0:507_0": "Vuttena tena vidhinā vidhinā tato taṃ,",
  "anya-e0607n0:508_0": "Laddhā nubhūtamamataṃ khila dosa nāsaṃ;",
  "anya-e0607n0:509_0": "Accanta roga jaratā maraṇā bhi bhūtaṃ,",
  "anya-e0607n0:510_0": "Bhūtaṃ karotu amaraṃ ajaraṃ arogaṃ.",
  "anya-e0607n0:511_0": "102.",
  "anya-e0607n0:512_0": "Saddhamma rāja raviniggata dhammaraṃsi,",
  "anya-e0607n0:513_0": "Phullo dhutaṅgadala saṃvara kesarāli;",
  "anya-e0607n0:514_0": "Saṅghāravinda nikaro samadhuṃ samādhi,",
  "anya-e0607n0:515_0": "Sakkiṇṇiko disatu sāsana vāpi jato.",
  "anya-e0607n0:516_0": "103.",
  "anya-e0607n0:517_0": "Ānanda rañña ratanādi mahā yatinda,",
  "anya-e0607n0:518_0": "Niccappabuddha padumappiya sevinaṅgī;",
  "anya-e0607n0:519_0": "Buddhappiyena ghana buddha guṇappiyena,",
  "anya-e0607n0:520_0": "Therālinā racita pajjamadhuṃ pibantu.",
  "anya-e0607n0:521_0": "104.",
  "anya-e0607n0:522_0": "Itthaṃ rūpa guṇānukittanavasā taṃ taṃ hitā siṃ sato,",
  "anya-e0607n0:523_0": "Vatthānussati vattita iha yathā sattesu mettā ca me;",
  "anya-e0607n0:524_0": "Evaṃ tābhi bhavanta ruttara tarā vattantu tā bodhi me,",
  "anya-e0607n0:525_0": "Saṃyogoca dhanehi santihi bhave kalyāna mittehi ca."
}