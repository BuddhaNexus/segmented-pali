{
  "kv15.3:0.1_0": "Kathāvatthu",
  "kv15.3:0.2_0": "Mahāpaṇṇāsaka",
  "kv15.3:0.3_0": "Pannarasamavagga",
  "kv15.3:0.4_0": "Addhākathā",
  "kv15.3:1.1_0": "Addhā parinipphannoti?",
  "kv15.3:1.2_0": "Āmantā.",
  "kv15.3:1.3_0": "Rūpanti?",
  "kv15.3:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv15.3:1.5_0": "vedanā …pe…",
  "kv15.3:1.6_0": "saññā …pe…",
  "kv15.3:1.7_0": "saṅkhārā …pe…",
  "kv15.3:1.8_0": "viññāṇanti?",
  "kv15.3:1.9_0": "Na hevaṁ vattabbe …pe…",
  "kv15.3:1.10_0": "atīto addhā parinipphannoti?",
  "kv15.3:1.11_0": "Āmantā.",
  "kv15.3:1.12_0": "Rūpanti?",
  "kv15.3:1.13_0": "Na hevaṁ vattabbe …pe…",
  "kv15.3:1.14_0": "vedanā …pe…",
  "kv15.3:1.15_0": "saññā …pe…",
  "kv15.3:1.16_0": "saṅkhārā …pe…",
  "kv15.3:1.17_0": "viññāṇanti?",
  "kv15.3:1.18_0": "Na hevaṁ vattabbe …pe…",
  "kv15.3:1.19_0": "anāgato addhā parinipphannoti?",
  "kv15.3:1.20_0": "Āmantā.",
  "kv15.3:1.21_0": "Rūpanti?",
  "kv15.3:1.22_0": "Na hevaṁ vattabbe …pe…",
  "kv15.3:1.23_0": "vedanā …pe…",
  "kv15.3:1.24_0": "saññā …pe…",
  "kv15.3:1.25_0": "saṅkhārā …pe…",
  "kv15.3:1.26_0": "viññāṇanti?",
  "kv15.3:1.27_0": "Na hevaṁ vattabbe …pe…",
  "kv15.3:1.28_0": "paccuppanno addhā parinipphannoti?",
  "kv15.3:1.29_0": "Āmantā.",
  "kv15.3:1.30_0": "Rūpanti?",
  "kv15.3:1.31_0": "Na hevaṁ vattabbe …pe…",
  "kv15.3:1.32_0": "vedanā …pe…",
  "kv15.3:1.33_0": "saññā …pe…",
  "kv15.3:1.34_0": "saṅkhārā …pe…",
  "kv15.3:1.35_0": "viññāṇanti?",
  "kv15.3:1.36_0": "Na hevaṁ vattabbe …pe….",
  "kv15.3:2.1_0": "Atītaṁ rūpaṁ vedanā saññā saṅkhārā viññāṇaṁ atīto addhāti?",
  "kv15.3:2.2_0": "Āmantā.",
  "kv15.3:2.3_0": "Atītā pañcaddhāti?",
  "kv15.3:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv15.3:2.5_0": "anāgataṁ rūpaṁ vedanā saññā saṅkhārā viññāṇaṁ anāgato addhāti?",
  "kv15.3:2.6_0": "Āmantā.",
  "kv15.3:2.7_0": "Anāgatā pañcaddhāti?",
  "kv15.3:2.8_0": "Na hevaṁ vattabbe …pe…",
  "kv15.3:2.9_0": "paccuppannaṁ rūpaṁ vedanā saññā saṅkhārā viññāṇaṁ paccuppanno addhāti?",
  "kv15.3:2.10_0": "Āmantā.",
  "kv15.3:2.11_0": "Paccuppannā pañcaddhāti?",
  "kv15.3:2.12_0": "Na hevaṁ vattabbe …pe….",
  "kv15.3:3.1_0": "Atītā pañcakkhandhā atīto addhā,",
  "kv15.3:3.1_1": "anāgatā pañcakkhandhā anāgato addhā,",
  "kv15.3:3.1_2": "paccuppannā pañcakkhandhā paccuppanno addhāti?",
  "kv15.3:3.2_0": "Āmantā.",
  "kv15.3:3.3_0": "Pannarasaddhāti?",
  "kv15.3:3.4_0": "Na hevaṁ vattabbe …pe….",
  "kv15.3:4.1_0": "Atītāni dvādasāyatanāni atīto addhā,",
  "kv15.3:4.1_1": "anāgatāni dvādasāyatanāni anāgato addhā,",
  "kv15.3:4.1_2": "paccuppannāni dvādasāyatanāni paccuppanno addhāti?",
  "kv15.3:4.2_0": "Āmantā.",
  "kv15.3:4.3_0": "Chattiṁsa addhāti?",
  "kv15.3:4.4_0": "Na hevaṁ vattabbe …pe….",
  "kv15.3:5.1_0": "Atītā aṭṭhārasa dhātuyo atīto addhā,",
  "kv15.3:5.1_1": "anāgatā aṭṭhārasa dhātuyo anāgato addhā,",
  "kv15.3:5.1_2": "paccuppannā aṭṭhārasa dhātuyo paccuppanno addhāti?",
  "kv15.3:5.2_0": "Āmantā.",
  "kv15.3:5.3_0": "Catupaññāsa addhāti?",
  "kv15.3:5.4_0": "Na hevaṁ vattabbe …pe….",
  "kv15.3:6.1_0": "Atītāni bāvīsatindriyāni atīto addhā,",
  "kv15.3:6.1_1": "anāgatāni bāvīsatindriyāni anāgato addhā,",
  "kv15.3:6.1_2": "paccuppannāni bāvīsatindriyāni paccuppanno addhāti?",
  "kv15.3:6.2_0": "Āmantā.",
  "kv15.3:6.3_0": "Chasaṭṭhi addhāti?",
  "kv15.3:6.4_0": "Na hevaṁ vattabbe …pe….",
  "kv15.3:7.1_0": "Na vattabbaṁ—",
  "kv15.3:7.2_0": "“addhā parinipphanno”ti?",
  "kv15.3:7.3_0": "Āmantā.",
  "kv15.3:7.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv15.3:7.5_0": "“tīṇimāni, bhikkhave, kathāvatthūni.",
  "kv15.3:7.6_0": "Katamāni tīṇi?",
  "kv15.3:7.7_0": "Atītaṁ vā, bhikkhave, addhānaṁ ārabbha kathaṁ katheyya—",
  "kv15.3:7.8_0": "‘evaṁ ahosi atītamaddhānan’ti;",
  "kv15.3:7.9_0": "anāgataṁ vā, bhikkhave, addhānaṁ ārabbha kathaṁ katheyya—",
  "kv15.3:7.10_0": "‘evaṁ bhavissati anāgatamaddhānan’ti;",
  "kv15.3:7.11_0": "etarahi vā, bhikkhave, paccuppannaṁ addhānaṁ ārabbha kathaṁ katheyya—",
  "kv15.3:7.12_0": "‘evaṁ hoti etarahi paccuppannan’ti.",
  "kv15.3:7.13_0": "Imāni kho, bhikkhave, tīṇi kathāvatthūnī”ti.",
  "kv15.3:7.14_0": "Attheva suttantoti?",
  "kv15.3:7.15_0": "Āmantā.",
  "kv15.3:7.16_0": "Tena hi addhā parinipphannoti.",
  "kv15.3:7.17_0": "Addhākathā niṭṭhitā."
}