{
  "atk-s0404a20:0_0": "(6) 1. Sacittavaggo",
  "atk-s0404a20:1_0": "1-4. Sacittasuttādivaṇṇanā",
  "atk-s0404a20:2_0": "51- 54.",
  "atk-s0404a20:2_1": "Dutiyassa paṭhame sacittapariyāyakusaloti attano cittavārakusalo.",
  "atk-s0404a20:2_2": "Rajanti āgantukaupakkilesaṁ.",
  "atk-s0404a20:2_3": "Aṅgaṇanti tatthajātakaaṅgakāḷatilakādiṁ.",
  "atk-s0404a20:2_4": "Āsavānaṁ khayāyāti arahattatthāya.",
  "atk-s0404a20:2_5": "Tatiye paṭibhānenāti vacanasaṇṭhānena.",
  "atk-s0404a20:2_6": "Catutthe adhipaññādhammavipassanāyāti saṅkhārapariggāhakavipassanāya.",
  "atk-s0404a20:3_0": "8. Mūlakasuttavaṇṇanā",
  "atk-s0404a20:4_0": "58.",
  "atk-s0404a20:4_1": "Aṭṭhame amatogadhāti ettha saupādisesā nibbānadhātu kathitā,",
  "atk-s0404a20:4_2": "nibbānapariyosānāti ettha anupādisesā.",
  "atk-s0404a20:4_3": "Anupādisesaṁ pattassa hi sabbe dhammā pariyosānappattā nāma honti.",
  "atk-s0404a20:4_4": "Sesapadāni heṭṭhā vuttatthāneva.",
  "atk-s0404a20:5_0": "9. Pabbajjāsuttavaṇṇanā",
  "atk-s0404a20:6_0": "59.",
  "atk-s0404a20:6_1": "Navame tasmāti yasmā evaṁ aparicitacittassa sāmaññattho na sampajjati, tasmā.",
  "atk-s0404a20:6_2": "Yathāpabbajjāparicitañca no cittaṁ bhavissatīti yathā pabbajjānurūpena paricitaṁ.",
  "atk-s0404a20:6_3": "Ye hi keci pabbajanti nāma,",
  "atk-s0404a20:6_4": "sabbe te arahattaṁ patthetvā.",
  "atk-s0404a20:6_5": "Tasmā yaṁ cittaṁ arahattādhigamatthāya paricitaṁ vaḍḍhitaṁ,",
  "atk-s0404a20:6_6": "taṁ yathāpabbajjāparicitaṁ nāmāti veditabbaṁ.",
  "atk-s0404a20:6_7": "Evarūpaṁ pana cittaṁ bhavissatīti sikkhitabbaṁ.",
  "atk-s0404a20:6_8": "Lokassa samañca visamañcāti sattalokassa sucaritaduccaritāni.",
  "atk-s0404a20:6_9": "Lokassa bhavañca vibhavañcāti tassa vaḍḍhiñca vināsañca,",
  "atk-s0404a20:6_10": "tathā sampattiñca vipattiñca.",
  "atk-s0404a20:6_11": "Lokassasamudayañca atthaṅgamañcāti pana saṅkhāralokaṁ sandhāya vuttaṁ,",
  "atk-s0404a20:6_12": "khandhānaṁ nibbattiñca bhedañcāti attho.",
  "atk-s0404a20:7_0": "10. Girimānandasuttavaṇṇanā",
  "atk-s0404a20:8_0": "60.",
  "atk-s0404a20:8_1": "Dasame anukampaṁ upādāyāti girimānandatthere anukampaṁ paṭicca.",
  "atk-s0404a20:8_2": "Cakkhurogotiādayo vatthuvasena veditabbā.",
  "atk-s0404a20:8_3": "Nibbattitappasādānañhi rogo nāma natthi.",
  "atk-s0404a20:8_4": "Kaṇṇarogoti bahikaṇṇe rogo.",
  "atk-s0404a20:8_5": "Pināsoti bahināsikāya rogo.",
  "atk-s0404a20:8_6": "Nakhasāti nakhehi vilekhitaṭṭhāne rogo.",
  "atk-s0404a20:8_7": "Pittasamuṭṭhānāti pittasamuṭṭhitā.",
  "atk-s0404a20:8_8": "Te kira dvattiṁsa honti.",
  "atk-s0404a20:8_9": "Semhasamuṭṭhānādīsupi eseva nayo.",
  "atk-s0404a20:8_10": "Utupariṇāmajāti utupariṇāmena accuṇhātisītena uppajjanakarogā.",
  "atk-s0404a20:8_11": "Visamaparihārajāti aticiraṭṭhānanisajjādinā visamaparihārena jātā.",
  "atk-s0404a20:8_12": "Opakkamikāti vadhabandhanādinā upakkamena jātā.",
  "atk-s0404a20:8_13": "Kammavipākajāti balavakammavipākasambhūtā.",
  "atk-s0404a20:8_14": "Santanti rāgādisantatāya santaṁ.",
  "atk-s0404a20:8_15": "Atappakaṭṭhena paṇītaṁ.",
  "atk-s0404a20:8_16": "Sesaṁ sabbattha uttānatthamevāti.",
  "atk-s0404a20:9_0": "Sacittavaggo paṭhamo."
}