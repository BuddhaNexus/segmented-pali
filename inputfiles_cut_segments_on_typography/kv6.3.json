{
  "kv6.3:0.1_0": "Kathāvatthu",
  "kv6.3:0.2_0": "Mahāpaṇṇāsaka",
  "kv6.3:0.3_0": "Chaṭṭhavagga",
  "kv6.3:0.4_0": "Saccakathā",
  "kv6.3:1.1_0": "Cattāri saccāni asaṅkhatānīti?",
  "kv6.3:1.2_0": "Āmantā.",
  "kv6.3:1.3_0": "Cattāri tāṇāni cattāri leṇāni cattāri saraṇāni cattāri parāyanāni cattāri accutāni cattāri amatāni cattāri nibbānānīti?",
  "kv6.3:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv6.3:1.5_0": "cattāri nibbānānīti?",
  "kv6.3:1.6_0": "Āmantā.",
  "kv6.3:1.7_0": "Atthi catunnaṁ nibbānānaṁ uccanīcatā hīnapaṇītatā ukkaṁsāvakaṁso sīmā vā bhedo vā rāji vā antarikā vāti?",
  "kv6.3:1.8_0": "Na hevaṁ vattabbe …pe….",
  "kv6.3:2.1_0": "Dukkhasaccaṁ asaṅkhatanti?",
  "kv6.3:2.2_0": "Āmantā.",
  "kv6.3:2.3_0": "Dukkhaṁ asaṅkhatanti?",
  "kv6.3:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv6.3:2.5_0": "dukkhasaccaṁ asaṅkhatanti?",
  "kv6.3:2.6_0": "Āmantā.",
  "kv6.3:2.7_0": "Kāyikaṁ dukkhaṁ cetasikaṁ dukkhaṁ sokaparidevadukkhadomanassaupāyāsā asaṅkhatāti?",
  "kv6.3:2.8_0": "Na hevaṁ vattabbe …pe…",
  "kv6.3:2.9_0": "samudayasaccaṁ asaṅkhatanti?",
  "kv6.3:2.10_0": "Āmantā.",
  "kv6.3:2.11_0": "Samudayo asaṅkhatoti?",
  "kv6.3:2.12_0": "Na hevaṁ vattabbe …pe…",
  "kv6.3:2.13_0": "samudayasaccaṁ asaṅkhatanti?",
  "kv6.3:2.14_0": "Āmantā.",
  "kv6.3:2.15_0": "Kāmataṇhā bhavataṇhā vibhavataṇhā asaṅkhatāti?",
  "kv6.3:2.16_0": "Na hevaṁ vattabbe …pe…",
  "kv6.3:2.17_0": "maggasaccaṁ asaṅkhatanti?",
  "kv6.3:2.18_0": "Āmantā.",
  "kv6.3:2.19_0": "Maggo asaṅkhatoti?",
  "kv6.3:2.20_0": "Na hevaṁ vattabbe …pe…",
  "kv6.3:2.21_0": "maggasaccaṁ asaṅkhatanti?",
  "kv6.3:2.22_0": "Āmantā.",
  "kv6.3:2.23_0": "sammādiṭṭhi …pe… sammāsamādhi asaṅkhatoti?",
  "kv6.3:2.24_0": "Na hevaṁ vattabbe …pe….",
  "kv6.3:3.1_0": "Dukkhaṁ saṅkhatanti?",
  "kv6.3:3.2_0": "Āmantā.",
  "kv6.3:3.3_0": "Dukkhasaccaṁ saṅkhatanti?",
  "kv6.3:3.4_0": "Na hevaṁ vattabbe …pe…",
  "kv6.3:3.5_0": "kāyikaṁ dukkhaṁ cetasikaṁ dukkhaṁ sokaparidevadukkhadomanassaupāyāsā saṅkhatāti?",
  "kv6.3:3.6_0": "Āmantā.",
  "kv6.3:3.7_0": "Dukkhasaccaṁ saṅkhatanti?",
  "kv6.3:3.8_0": "Na hevaṁ vattabbe …pe…",
  "kv6.3:3.9_0": "samudayo saṅkhatoti?",
  "kv6.3:3.10_0": "Āmantā.",
  "kv6.3:3.11_0": "Samudayasaccaṁ saṅkhatanti?",
  "kv6.3:3.12_0": "Na hevaṁ vattabbe …pe…",
  "kv6.3:3.13_0": "kāmataṇhā bhavataṇhā vibhavataṇhā saṅkhatāti?",
  "kv6.3:3.14_0": "Āmantā.",
  "kv6.3:3.15_0": "Samudayasaccaṁ saṅkhatanti?",
  "kv6.3:3.16_0": "Na hevaṁ vattabbe …pe…",
  "kv6.3:3.17_0": "maggo saṅkhatoti?",
  "kv6.3:3.18_0": "Āmantā.",
  "kv6.3:3.19_0": "Maggasaccaṁ saṅkhatanti?",
  "kv6.3:3.20_0": "Na hevaṁ vattabbe …pe…",
  "kv6.3:3.21_0": "sammādiṭṭhi …pe… sammāsamādhi saṅkhatoti?",
  "kv6.3:3.22_0": "Āmantā.",
  "kv6.3:3.23_0": "Maggasaccaṁ saṅkhatanti?",
  "kv6.3:3.24_0": "Na hevaṁ vattabbe …pe….",
  "kv6.3:4.1_0": "Nirodhasaccaṁ asaṅkhataṁ, nirodho asaṅkhatoti?",
  "kv6.3:4.2_0": "Āmantā.",
  "kv6.3:4.3_0": "Dukkhasaccaṁ asaṅkhataṁ, dukkhaṁ asaṅkhatanti?",
  "kv6.3:4.4_0": "Na hevaṁ vattabbe …pe…",
  "kv6.3:4.5_0": "nirodhasaccaṁ asaṅkhataṁ, nirodho asaṅkhatoti?",
  "kv6.3:4.6_0": "Āmantā.",
  "kv6.3:4.7_0": "Samudayasaccaṁ asaṅkhataṁ, samudayo asaṅkhatoti?",
  "kv6.3:4.8_0": "Na hevaṁ vattabbe …pe…",
  "kv6.3:4.9_0": "nirodhasaccaṁ asaṅkhataṁ, nirodho asaṅkhatoti?",
  "kv6.3:4.10_0": "Āmantā.",
  "kv6.3:4.11_0": "Maggasaccaṁ asaṅkhataṁ, maggo asaṅkhatoti?",
  "kv6.3:4.12_0": "Na hevaṁ vattabbe …pe….",
  "kv6.3:5.1_0": "Dukkhasaccaṁ asaṅkhataṁ, dukkhaṁ saṅkhatanti?",
  "kv6.3:5.2_0": "Āmantā.",
  "kv6.3:5.3_0": "Nirodhasaccaṁ asaṅkhataṁ, nirodho saṅkhatoti?",
  "kv6.3:5.4_0": "Na hevaṁ vattabbe …pe…",
  "kv6.3:5.5_0": "samudayasaccaṁ asaṅkhataṁ, samudayo saṅkhatoti?",
  "kv6.3:5.6_0": "Āmantā.",
  "kv6.3:5.7_0": "Nirodhasaccaṁ asaṅkhataṁ, nirodho saṅkhatoti?",
  "kv6.3:5.8_0": "Na hevaṁ vattabbe …pe…",
  "kv6.3:5.9_0": "maggasaccaṁ asaṅkhataṁ, maggo saṅkhatoti?",
  "kv6.3:5.10_0": "Āmantā.",
  "kv6.3:5.11_0": "Nirodhasaccaṁ asaṅkhataṁ, nirodho saṅkhatoti?",
  "kv6.3:5.12_0": "Na hevaṁ vattabbe …pe….",
  "kv6.3:6.1_0": "Na vattabbaṁ—",
  "kv6.3:6.2_0": "“cattāri saccāni asaṅkhatānī”ti?",
  "kv6.3:6.3_0": "Āmantā.",
  "kv6.3:6.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv6.3:6.5_0": "“cattārimāni, bhikkhave, tathāni avitathāni anaññathāni.",
  "kv6.3:6.6_0": "Katamāni cattāri?",
  "kv6.3:6.7_0": "‘Idaṁ dukkhan’ti, bhikkhave, tathametaṁ avitathametaṁ anaññathametaṁ …pe…",
  "kv6.3:6.8_0": "‘ayaṁ dukkhasamudayo’ti …pe…",
  "kv6.3:6.9_0": "‘ayaṁ dukkhanirodho’ti …pe…",
  "kv6.3:6.10_0": "‘ayaṁ dukkhanirodhagāminī paṭipadā’ti tathametaṁ avitathametaṁ anaññathametaṁ.",
  "kv6.3:6.11_0": "Imāni kho, bhikkhave, cattāri tathāni avitathāni anaññathānī”ti.",
  "kv6.3:6.12_0": "Attheva suttantoti?",
  "kv6.3:6.13_0": "Āmantā.",
  "kv6.3:6.14_0": "Tena hi cattāri saccāni asaṅkhatānīti.",
  "kv6.3:6.15_0": "Saccakathā niṭṭhitā."
}