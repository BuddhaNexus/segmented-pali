{
  "tha-ap229:0.1_0": "Therāpadāna",
  "tha-ap229:0.2_0": "Ālambaṇadāyakavagga",
  "tha-ap229:0.3_0": "7. Sovaṇṇavaṭaṁsakiyattheraapadāna",
  "tha-ap229:1.1_0": "“Uyyānabhūmiṁ niyyanto,",
  "tha-ap229:1.2_0": "addasaṁ lokanāyakaṁ;",
  "tha-ap229:1.3_0": "Vaṭaṁsakaṁ gahetvāna,",
  "tha-ap229:1.4_0": "sovaṇṇaṁ sādhunimmitaṁ.",
  "tha-ap229:2.1_0": "Sīghaṁ tato samāruyha,",
  "tha-ap229:2.2_0": "hatthikkhandhagato ahaṁ;",
  "tha-ap229:2.3_0": "Buddhassa abhiropesiṁ,",
  "tha-ap229:2.4_0": "sikhino lokabandhuno.",
  "tha-ap229:3.1_0": "Ekattiṁse ito kappe,",
  "tha-ap229:3.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap229:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap229:3.4_0": "pupphapūjāyidaṁ phalaṁ.",
  "tha-ap229:4.1_0": "Sattavīse ito kappe,",
  "tha-ap229:4.2_0": "eko āsiṁ janādhipo;",
  "tha-ap229:4.3_0": "Mahāpatāpanāmena,",
  "tha-ap229:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap229:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap229:5.2_0": "…pe…",
  "tha-ap229:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap229:6.1_0": "Itthaṁ sudaṁ āyasmā sovaṇṇavaṭaṁsakiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap229:7.1_0": "Sovaṇṇavaṭaṁsakiyattherassāpadānaṁ sattamaṁ."
}