{
  "thag16.10:0.1_0": "Theragāthā 16.10",
  "thag16.10:0.2_0": "Vīsatinipāta",
  "thag16.10:0.3_0": "Paṭhamavagga",
  "thag16.10:0.4_0": "Pārāpariyattheragāthā",
  "thag16.10:1.1_0": "Samaṇassa ahu cintā,",
  "thag16.10:1.2_0": "pupphitamhi mahāvane;",
  "thag16.10:1.3_0": "Ekaggassa nisinnassa,",
  "thag16.10:1.4_0": "pavivittassa jhāyino.",
  "thag16.10:2.1_0": "“Aññathā lokanāthamhi,",
  "thag16.10:2.2_0": "tiṭṭhante purisuttame;",
  "thag16.10:2.3_0": "Iriyaṁ āsi bhikkhūnaṁ,",
  "thag16.10:2.4_0": "aññathā dāni dissati.",
  "thag16.10:3.1_0": "Sītavātaparittāṇaṁ,",
  "thag16.10:3.2_0": "hirikopīnachādanaṁ;",
  "thag16.10:3.3_0": "Mattaṭṭhiyaṁ abhuñjiṁsu,",
  "thag16.10:3.4_0": "santuṭṭhā itarītare.",
  "thag16.10:4.1_0": "Paṇītaṁ yadi vā lūkhaṁ,",
  "thag16.10:4.2_0": "appaṁ vā yadi vā bahuṁ;",
  "thag16.10:4.3_0": "Yāpanatthaṁ abhuñjiṁsu,",
  "thag16.10:4.4_0": "agiddhā nādhimucchitā.",
  "thag16.10:5.1_0": "Jīvitānaṁ parikkhāre,",
  "thag16.10:5.2_0": "bhesajje atha paccaye;",
  "thag16.10:5.3_0": "Na bāḷhaṁ ussukā āsuṁ,",
  "thag16.10:5.4_0": "yathā te āsavakkhaye.",
  "thag16.10:6.1_0": "Araññe rukkhamūlesu,",
  "thag16.10:6.2_0": "kandarāsu guhāsu ca;",
  "thag16.10:6.3_0": "Vivekamanubrūhantā,",
  "thag16.10:6.4_0": "vihaṁsu tapparāyaṇā.",
  "thag16.10:7.1_0": "Nīcā niviṭṭhā subharā,",
  "thag16.10:7.2_0": "mudū atthaddhamānasā;",
  "thag16.10:7.3_0": "Abyāsekā amukharā,",
  "thag16.10:7.4_0": "atthacintā vasānugā.",
  "thag16.10:8.1_0": "Tato pāsādikaṁ āsi,",
  "thag16.10:8.2_0": "gataṁ bhuttaṁ nisevitaṁ;",
  "thag16.10:8.3_0": "Siniddhā teladhārāva,",
  "thag16.10:8.4_0": "ahosi iriyāpatho.",
  "thag16.10:9.1_0": "Sabbāsavaparikkhīṇā,",
  "thag16.10:9.2_0": "mahājhāyī mahāhitā;",
  "thag16.10:9.3_0": "Nibbutā dāni te therā,",
  "thag16.10:9.4_0": "parittā dāni tādisā.",
  "thag16.10:10.1_0": "Kusalānañca dhammānaṁ,",
  "thag16.10:10.2_0": "paññāya ca parikkhayā;",
  "thag16.10:10.3_0": "Sabbākāravarūpetaṁ,",
  "thag16.10:10.4_0": "lujjate jinasāsanaṁ.",
  "thag16.10:11.1_0": "Pāpakānañca dhammānaṁ,",
  "thag16.10:11.2_0": "kilesānañca yo utu;",
  "thag16.10:11.3_0": "Upaṭṭhitā vivekāya,",
  "thag16.10:11.4_0": "ye ca saddhammasesakā.",
  "thag16.10:12.1_0": "Te kilesā pavaḍḍhantā,",
  "thag16.10:12.2_0": "āvisanti bahuṁ janaṁ;",
  "thag16.10:12.3_0": "Kīḷanti maññe bālehi,",
  "thag16.10:12.4_0": "ummattehiva rakkhasā.",
  "thag16.10:13.1_0": "Kilesehābhibhūtā te,",
  "thag16.10:13.2_0": "tena tena vidhāvitā;",
  "thag16.10:13.3_0": "Narā kilesavatthūsu,",
  "thag16.10:13.4_0": "sasaṅgāmeva ghosite.",
  "thag16.10:14.1_0": "Pariccajitvā saddhammaṁ,",
  "thag16.10:14.2_0": "aññamaññehi bhaṇḍare;",
  "thag16.10:14.3_0": "Diṭṭhigatāni anventā,",
  "thag16.10:14.4_0": "idaṁ seyyoti maññare.",
  "thag16.10:15.1_0": "Dhanañca puttaṁ bhariyañca,",
  "thag16.10:15.2_0": "chaḍḍayitvāna niggatā;",
  "thag16.10:15.3_0": "Kaṭacchubhikkhahetūpi,",
  "thag16.10:15.4_0": "akicchāni nisevare.",
  "thag16.10:16.1_0": "Udarāvadehakaṁ bhutvā,",
  "thag16.10:16.2_0": "sayantuttānaseyyakā;",
  "thag16.10:16.3_0": "Kathaṁ vattenti paṭibuddhā,",
  "thag16.10:16.4_0": "yā kathā satthugarahitā.",
  "thag16.10:17.1_0": "Sabbakārukasippāni,",
  "thag16.10:17.2_0": "cittiṁ katvāna sikkhare;",
  "thag16.10:17.3_0": "Avūpasantā ajjhattaṁ,",
  "thag16.10:17.4_0": "sāmaññatthotiacchati.",
  "thag16.10:18.1_0": "Mattikaṁ telacuṇṇañca,",
  "thag16.10:18.2_0": "udakāsanabhojanaṁ;",
  "thag16.10:18.3_0": "Gihīnaṁ upanāmenti,",
  "thag16.10:18.4_0": "ākaṅkhantā bahuttaraṁ.",
  "thag16.10:19.1_0": "Dantaponaṁ kapitthañca,",
  "thag16.10:19.2_0": "pupphaṁ khādaniyāni ca;",
  "thag16.10:19.3_0": "Piṇḍapāte ca sampanne,",
  "thag16.10:19.4_0": "ambe āmalakāni ca.",
  "thag16.10:20.1_0": "Bhesajjesu yathā vejjā,",
  "thag16.10:20.2_0": "kiccākicce yathā gihī;",
  "thag16.10:20.3_0": "Gaṇikāva vibhūsāyaṁ,",
  "thag16.10:20.4_0": "issare khattiyā yathā.",
  "thag16.10:21.1_0": "Nekatikā vañcanikā,",
  "thag16.10:21.2_0": "kūṭasakkhī apāṭukā;",
  "thag16.10:21.3_0": "Bahūhi parikappehi,",
  "thag16.10:21.4_0": "āmisaṁ paribhuñjare.",
  "thag16.10:22.1_0": "Lesakappe pariyāye,",
  "thag16.10:22.2_0": "parikappenudhāvitā;",
  "thag16.10:22.3_0": "Jīvikatthā upāyena,",
  "thag16.10:22.4_0": "saṅkaḍḍhanti bahuṁ dhanaṁ.",
  "thag16.10:23.1_0": "Upaṭṭhāpenti parisaṁ,",
  "thag16.10:23.2_0": "kammato no ca dhammato;",
  "thag16.10:23.3_0": "Dhammaṁ paresaṁ desenti,",
  "thag16.10:23.4_0": "lābhato no ca atthato.",
  "thag16.10:24.1_0": "Saṅghalābhassa bhaṇḍanti,",
  "thag16.10:24.2_0": "saṅghato paribāhirā;",
  "thag16.10:24.3_0": "Paralābhopajīvantā,",
  "thag16.10:24.4_0": "ahirīkā na lajjare.",
  "thag16.10:25.1_0": "Nānuyuttā tathā eke,",
  "thag16.10:25.2_0": "muṇḍā saṅghāṭipārutā;",
  "thag16.10:25.3_0": "Sambhāvanaṁyevicchanti,",
  "thag16.10:25.4_0": "lābhasakkāramucchitā.",
  "thag16.10:26.1_0": "Evaṁ nānappayātamhi,",
  "thag16.10:26.2_0": "na dāni sukaraṁ tathā;",
  "thag16.10:26.3_0": "Aphusitaṁ vā phusituṁ,",
  "thag16.10:26.4_0": "phusitaṁ vānurakkhituṁ.",
  "thag16.10:27.1_0": "Yathā kaṇṭakaṭṭhānamhi,",
  "thag16.10:27.2_0": "careyya anupāhano;",
  "thag16.10:27.3_0": "Satiṁ upaṭṭhapetvāna,",
  "thag16.10:27.4_0": "evaṁ gāme munī care.",
  "thag16.10:28.1_0": "Saritvā pubbake yogī,",
  "thag16.10:28.2_0": "tesaṁ vattamanussaraṁ;",
  "thag16.10:28.3_0": "Kiñcāpi pacchimo kālo,",
  "thag16.10:28.4_0": "phuseyya amataṁ padaṁ.",
  "thag16.10:29.1_0": "Idaṁ vatvā sālavane,",
  "thag16.10:29.2_0": "samaṇo bhāvitindriyo;",
  "thag16.10:29.3_0": "Brāhmaṇo parinibbāyī,",
  "thag16.10:29.4_0": "isi khīṇapunabbhavo”ti.",
  "thag16.10:29.5_0": "… Pārāpariyo thero …",
  "thag16.10:29.6_0": "Vīsatinipāto niṭṭhito.",
  "thag16.10:30.0_0": "Tatruddānaṁ",
  "thag16.10:30.1_0": "Adhimutto pārāpariyo,",
  "thag16.10:30.2_0": "telakāni raṭṭhapālo;",
  "thag16.10:30.3_0": "Mālukyaselo bhaddiyo,",
  "thag16.10:30.4_0": "aṅguli dibbacakkhuko;",
  "thag16.10:30.5_0": "Pārāpariyo dasete,",
  "thag16.10:30.6_0": "vīsamhi parikittitā;",
  "thag16.10:30.7_0": "Gāthāyo dve satā honti,",
  "thag16.10:30.8_0": "pañcatālīsa uttarinti."
}