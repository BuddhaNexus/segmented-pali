{
  "pli-tv-pvr2.9:0.1_0": "Parivāra",
  "pli-tv-pvr2.9:0.2_0": "Bhikkhunivibhaṅga",
  "pli-tv-pvr2.9:0.3_0": "Dutiyabhāga",
  "pli-tv-pvr2.9:0.4_0": "Katthapaññattivāra",
  "pli-tv-pvr2.9:1.0_0": "1. Pārājikakaṇḍa",
  "pli-tv-pvr2.9:1.1_0": "Yaṁ tena bhagavatā jānatā passatā arahatā sammāsambuddhena kāyasaṁsaggaṁ sādiyanapaccayā pārājikaṁ kattha paññattaṁ?",
  "pli-tv-pvr2.9:1.2_0": "Kaṁ ārabbha?",
  "pli-tv-pvr2.9:1.3_0": "Kismiṁ vatthusmiṁ …pe…",
  "pli-tv-pvr2.9:1.4_0": "kenābhatanti?",
  "pli-tv-pvr2.9:2.1_0": "Yaṁ tena bhagavatā jānatā passatā arahatā sammāsambuddhena kāyasaṁsaggaṁ sādiyanapaccayā pārājikaṁ kattha paññattanti?",
  "pli-tv-pvr2.9:2.2_0": "Sāvatthiyaṁ paññattaṁ.",
  "pli-tv-pvr2.9:2.3_0": "Kaṁ ārabbhāti?",
  "pli-tv-pvr2.9:2.4_0": "Sundarīnandaṁ bhikkhuniṁ ārabbha.",
  "pli-tv-pvr2.9:2.5_0": "Kismiṁ vatthusminti?",
  "pli-tv-pvr2.9:2.6_0": "Sundarīnandā bhikkhunī avassutā avassutassa purisapuggalassa kāyasaṁsaggaṁ sādiyi,",
  "pli-tv-pvr2.9:2.6_1": "tasmiṁ vatthusmiṁ.",
  "pli-tv-pvr2.9:2.7_0": "Atthi tattha paññatti, anupaññatti, anuppannapaññattīti?",
  "pli-tv-pvr2.9:2.8_0": "Ekā paññatti.",
  "pli-tv-pvr2.9:2.9_0": "Anupaññatti anuppannapaññatti tasmiṁ natthi.",
  "pli-tv-pvr2.9:2.10_0": "Sabbatthapaññatti, padesapaññattīti?",
  "pli-tv-pvr2.9:2.11_0": "Sabbatthapaññatti.",
  "pli-tv-pvr2.9:2.12_0": "Sādhāraṇapaññatti, asādhāraṇapaññattīti?",
  "pli-tv-pvr2.9:2.13_0": "Asādhāraṇapaññatti.",
  "pli-tv-pvr2.9:2.14_0": "Ekatopaññatti, ubhatopaññattīti?",
  "pli-tv-pvr2.9:2.15_0": "Ekatopaññatti.",
  "pli-tv-pvr2.9:2.16_0": "Catunnaṁ pātimokkhuddesānaṁ katthogadhaṁ kattha pariyāpannanti?",
  "pli-tv-pvr2.9:2.17_0": "Nidānogadhaṁ nidānapariyāpannaṁ?",
  "pli-tv-pvr2.9:2.18_0": "Katamena uddesena uddesaṁ āgacchatīti?",
  "pli-tv-pvr2.9:2.19_0": "Dutiyena uddesena uddesaṁ āgacchati.",
  "pli-tv-pvr2.9:2.20_0": "Catunnaṁ vipattīnaṁ katamā vipattīti?",
  "pli-tv-pvr2.9:2.21_0": "Sīlavipatti.",
  "pli-tv-pvr2.9:2.22_0": "Sattannaṁ āpattikkhandhānaṁ katamo āpattikkhandhoti?",
  "pli-tv-pvr2.9:2.23_0": "Pārājikāpattikkhandho.",
  "pli-tv-pvr2.9:2.24_0": "Channaṁ āpattisamuṭṭhānānaṁ katihi samuṭṭhānehi samuṭṭhātīti?",
  "pli-tv-pvr2.9:2.25_0": "Ekena samuṭṭhānena samuṭṭhāti—",
  "pli-tv-pvr2.9:2.26_0": "kāyato ca cittato ca samuṭṭhāti, na vācato …pe…",
  "pli-tv-pvr2.9:2.27_0": "kenābhatanti?",
  "pli-tv-pvr2.9:2.28_0": "Paramparābhataṁ—",
  "pli-tv-pvr2.9:3.1_0": "Upāli dāsako ceva,",
  "pli-tv-pvr2.9:3.2_0": "soṇako siggavo tathā;",
  "pli-tv-pvr2.9:3.3_0": "Moggaliputtena pañcamā,",
  "pli-tv-pvr2.9:3.4_0": "ete jambusirivhaye.",
  "pli-tv-pvr2.9:4.1_0": "…pe…",
  "pli-tv-pvr2.9:5.1_0": "Ete nāgā mahāpaññā,",
  "pli-tv-pvr2.9:5.2_0": "Vinayaññū maggakovidā;",
  "pli-tv-pvr2.9:5.3_0": "Vinayaṁ dīpe pakāsesuṁ,",
  "pli-tv-pvr2.9:5.4_0": "Piṭakaṁ tambapaṇṇiyāti.",
  "pli-tv-pvr2.9:6.1_0": "Vajjappaṭicchādanapaccayā pārājikaṁ kattha paññattanti?",
  "pli-tv-pvr2.9:6.2_0": "Sāvatthiyaṁ paññattaṁ.",
  "pli-tv-pvr2.9:6.3_0": "Kaṁ ārabbhāti?",
  "pli-tv-pvr2.9:6.4_0": "Thullanandaṁ bhikkhuniṁ ārabbha.",
  "pli-tv-pvr2.9:6.5_0": "Kismiṁ vatthusminti?",
  "pli-tv-pvr2.9:6.6_0": "Thullanandā bhikkhunī jānaṁ pārājikaṁ dhammaṁ ajjhāpannaṁ bhikkhuniṁ nevattanā paṭicodesi na gaṇassa ārocesi,",
  "pli-tv-pvr2.9:6.6_1": "tasmiṁ vatthusmiṁ.",
  "pli-tv-pvr2.9:6.7_0": "Ekā paññatti.",
  "pli-tv-pvr2.9:6.8_0": "Channaṁ āpattisamuṭṭhānānaṁ ekena samuṭṭhānena samuṭṭhāti—",
  "pli-tv-pvr2.9:6.9_0": "dhuranikkhepe …pe….",
  "pli-tv-pvr2.9:7.1_0": "Yāvatatiyaṁ samanubhāsanāya na paṭinissajjanapaccayā pārājikaṁ kattha paññattanti?",
  "pli-tv-pvr2.9:7.2_0": "Sāvatthiyaṁ paññattaṁ.",
  "pli-tv-pvr2.9:7.3_0": "Kaṁ ārabbhāti?",
  "pli-tv-pvr2.9:7.4_0": "Thullanandaṁ bhikkhuniṁ ārabbha.",
  "pli-tv-pvr2.9:7.5_0": "Kismiṁ vatthusminti?",
  "pli-tv-pvr2.9:7.6_0": "Thullanandā bhikkhunī samaggena saṅghena ukkhittaṁ ariṭṭhaṁ bhikkhuṁ gaddhabādhipubbaṁ anuvatti,",
  "pli-tv-pvr2.9:7.6_1": "tasmiṁ vatthusmiṁ.",
  "pli-tv-pvr2.9:7.7_0": "Ekā paññatti.",
  "pli-tv-pvr2.9:7.8_0": "Channaṁ āpattisamuṭṭhānānaṁ ekena samuṭṭhānena samuṭṭhāti—",
  "pli-tv-pvr2.9:7.9_0": "dhuranikkhepe …pe….",
  "pli-tv-pvr2.9:8.1_0": "Aṭṭhamaṁ vatthuṁ paripūraṇapaccayā pārājikaṁ kattha paññattanti?",
  "pli-tv-pvr2.9:8.2_0": "Sāvatthiyaṁ paññattaṁ.",
  "pli-tv-pvr2.9:8.3_0": "Kaṁ ārabbhāti?",
  "pli-tv-pvr2.9:8.4_0": "Chabbaggiyā bhikkhuniyo ārabbha.",
  "pli-tv-pvr2.9:8.5_0": "Kismiṁ vatthusminti?",
  "pli-tv-pvr2.9:8.6_0": "Chabbaggiyā bhikkhuniyo aṭṭhamaṁ vatthuṁ paripūresuṁ, tasmiṁ vatthusmiṁ.",
  "pli-tv-pvr2.9:8.7_0": "Ekā paññatti.",
  "pli-tv-pvr2.9:8.8_0": "Channaṁ āpattisamuṭṭhānānaṁ ekena samuṭṭhānena samuṭṭhāti—",
  "pli-tv-pvr2.9:8.9_0": "dhuranikkhepe …pe….",
  "pli-tv-pvr2.9:8.10_0": "Pārājikā niṭṭhitā.",
  "pli-tv-pvr2.9:9.0_0": "2. Saṅghādisesakaṇḍādi",
  "pli-tv-pvr2.9:9.1_0": "Yaṁ tena bhagavatā jānatā passatā arahatā sammāsambuddhena ussayavādikāya bhikkhuniyā aḍḍaṁ karaṇapaccayā saṅghādiseso kattha paññatto?",
  "pli-tv-pvr2.9:9.2_0": "Kaṁ ārabbha?",
  "pli-tv-pvr2.9:9.3_0": "Kismiṁ vatthusmiṁ …pe…",
  "pli-tv-pvr2.9:9.4_0": "kenābhatanti?",
  "pli-tv-pvr2.9:10.1_0": "Yaṁ tena bhagavatā jānatā passatā arahatā sammāsambuddhena ussayavādikāya bhikkhuniyā aḍḍaṁ karaṇapaccayā saṅghādiseso kattha paññattoti?",
  "pli-tv-pvr2.9:10.2_0": "Sāvatthiyaṁ paññatto.",
  "pli-tv-pvr2.9:10.3_0": "Kaṁ ārabbhāti?",
  "pli-tv-pvr2.9:10.4_0": "Thullanandaṁ bhikkhuniṁ ārabbha.",
  "pli-tv-pvr2.9:10.5_0": "Kismiṁ vatthusminti?",
  "pli-tv-pvr2.9:10.6_0": "Thullanandā bhikkhunī ussayavādikā vihari, tasmiṁ vatthusmiṁ.",
  "pli-tv-pvr2.9:10.7_0": "Atthi tattha paññatti, anupaññatti, anuppannapaññattīti?",
  "pli-tv-pvr2.9:10.8_0": "Ekā paññatti.",
  "pli-tv-pvr2.9:10.9_0": "Anupaññatti anuppannapaññatti tasmiṁ natthi.",
  "pli-tv-pvr2.9:10.10_0": "Sabbatthapaññatti, padesapaññattīti?",
  "pli-tv-pvr2.9:10.11_0": "Sabbatthapaññatti.",
  "pli-tv-pvr2.9:10.12_0": "Sādhāraṇapaññatti, asādhāraṇapaññattīti?",
  "pli-tv-pvr2.9:10.13_0": "Asādhāraṇapaññatti.",
  "pli-tv-pvr2.9:10.14_0": "Ekatopaññatti, ubhatopaññattīti?",
  "pli-tv-pvr2.9:10.15_0": "Ekatopaññatti.",
  "pli-tv-pvr2.9:10.16_0": "Catunnaṁ pātimokkhuddesānaṁ katthogadhaṁ kattha pariyāpannanti?",
  "pli-tv-pvr2.9:10.17_0": "Nidānogadhaṁ nidānapariyāpannaṁ.",
  "pli-tv-pvr2.9:10.18_0": "Katamena uddesena uddesaṁ āgacchatīti?",
  "pli-tv-pvr2.9:10.19_0": "Tatiyena uddesena uddesaṁ āgacchati.",
  "pli-tv-pvr2.9:10.20_0": "Catunnaṁ vipattīnaṁ katamā vipattīti?",
  "pli-tv-pvr2.9:10.21_0": "Sīlavipatti.",
  "pli-tv-pvr2.9:10.22_0": "Sattannaṁ āpattikkhandhānaṁ katamo āpattikkhandhoti?",
  "pli-tv-pvr2.9:10.23_0": "Saṅghādisesāpattikkhandho.",
  "pli-tv-pvr2.9:10.24_0": "Channaṁ āpattisamuṭṭhānānaṁ katihi samuṭṭhānehi samuṭṭhātīti?",
  "pli-tv-pvr2.9:10.25_0": "Dvīhi samuṭṭhānehi samuṭṭhāti—",
  "pli-tv-pvr2.9:10.26_0": "siyā kāyato ca vācato ca samuṭṭhāti, na cittato;",
  "pli-tv-pvr2.9:10.27_0": "siyā kāyato ca vācato ca cittato ca samuṭṭhāti …pe…",
  "pli-tv-pvr2.9:10.28_0": "kenābhatanti?",
  "pli-tv-pvr2.9:10.29_0": "Paramparābhataṁ—",
  "pli-tv-pvr2.9:11.1_0": "Upāli dāsako ceva,",
  "pli-tv-pvr2.9:11.2_0": "soṇako siggavo tathā;",
  "pli-tv-pvr2.9:11.3_0": "Moggaliputtena pañcamā,",
  "pli-tv-pvr2.9:11.4_0": "ete jambusirivhaye.",
  "pli-tv-pvr2.9:12.1_0": "…pe…",
  "pli-tv-pvr2.9:13.1_0": "Ete nāgā mahāpaññā,",
  "pli-tv-pvr2.9:13.2_0": "Vinayaññū maggakovidā;",
  "pli-tv-pvr2.9:13.3_0": "Vinayaṁ dīpe pakāsesuṁ,",
  "pli-tv-pvr2.9:13.4_0": "Piṭakaṁ tambapaṇṇiyāti.",
  "pli-tv-pvr2.9:14.1_0": "Coriṁ vuṭṭhāpanapaccayā saṅghādiseso kattha paññattoti?",
  "pli-tv-pvr2.9:14.2_0": "Sāvatthiyaṁ paññatto.",
  "pli-tv-pvr2.9:14.3_0": "Kaṁ ārabbhāti?",
  "pli-tv-pvr2.9:14.4_0": "Thullanandaṁ bhikkhuniṁ ārabbha.",
  "pli-tv-pvr2.9:14.5_0": "Kismiṁ vatthusminti?",
  "pli-tv-pvr2.9:14.6_0": "Thullanandā bhikkhunī coriṁ vuṭṭhāpesi, tasmiṁ vatthusmiṁ.",
  "pli-tv-pvr2.9:14.7_0": "Ekā paññatti.",
  "pli-tv-pvr2.9:14.8_0": "Channaṁ āpattisamuṭṭhānānaṁ dvīhi samuṭṭhānehi samuṭṭhāti—",
  "pli-tv-pvr2.9:14.9_0": "siyā vācato ca cittato ca samuṭṭhāti, na kāyato;",
  "pli-tv-pvr2.9:14.10_0": "siyā kāyato ca vācato ca cittato ca samuṭṭhāti …pe….",
  "pli-tv-pvr2.9:15.1_0": "Ekā gāmantaraṁ gamanapaccayā saṅghādiseso kattha paññattoti?",
  "pli-tv-pvr2.9:15.2_0": "Sāvatthiyaṁ paññatto.",
  "pli-tv-pvr2.9:15.3_0": "Kaṁ ārabbhāti?",
  "pli-tv-pvr2.9:15.4_0": "Aññataraṁ bhikkhuniṁ ārabbha.",
  "pli-tv-pvr2.9:15.5_0": "Kismiṁ vatthusminti?",
  "pli-tv-pvr2.9:15.6_0": "Aññatarā bhikkhunī ekā gāmantaraṁ gacchi, tasmiṁ vatthusmiṁ.",
  "pli-tv-pvr2.9:15.7_0": "Ekā paññatti, tisso anupaññattiyo.",
  "pli-tv-pvr2.9:15.8_0": "Channaṁ āpattisamuṭṭhānānaṁ ekena samuṭṭhānena samuṭṭhāti—",
  "pli-tv-pvr2.9:15.9_0": "paṭhamapārājike …pe….",
  "pli-tv-pvr2.9:16.1_0": "Samaggena saṅghena ukkhittaṁ bhikkhuniṁ dhammena vinayena satthusāsanena anapaloketvā kārakasaṅghaṁ anaññāya",
  "pli-tv-pvr2.9:16.1_1": "gaṇassa chandaṁ osāraṇapaccayā saṅghādiseso kattha paññattoti?",
  "pli-tv-pvr2.9:16.2_0": "Sāvatthiyaṁ paññatto.",
  "pli-tv-pvr2.9:16.3_0": "Kaṁ ārabbhāti?",
  "pli-tv-pvr2.9:16.4_0": "Thullanandaṁ bhikkhuniṁ ārabbha.",
  "pli-tv-pvr2.9:16.5_0": "Kismiṁ vatthusminti?",
  "pli-tv-pvr2.9:16.6_0": "Thullanandā bhikkhunī samaggena saṅghena ukkhittaṁ bhikkhuniṁ dhammena vinayena satthusāsanena anapaloketvā",
  "pli-tv-pvr2.9:16.6_1": "kārakasaṅghaṁ anaññāya gaṇassa chandaṁ osāresi,",
  "pli-tv-pvr2.9:16.6_2": "tasmiṁ vatthusmiṁ.",
  "pli-tv-pvr2.9:16.7_0": "Ekā paññatti.",
  "pli-tv-pvr2.9:16.8_0": "Channaṁ āpattisamuṭṭhānānaṁ ekena samuṭṭhānena samuṭṭhāti—",
  "pli-tv-pvr2.9:16.9_0": "dhuranikkhepe …pe….",
  "pli-tv-pvr2.9:17.1_0": "Avassutāya bhikkhuniyā avassutassa purisapuggalassa hatthato khādanīyaṁ vā bhojanīyaṁ vā sahatthā paṭiggahetvā",
  "pli-tv-pvr2.9:17.1_1": "bhuñjanapaccayā saṅghādiseso kattha paññattoti?",
  "pli-tv-pvr2.9:17.2_0": "Sāvatthiyaṁ paññatto.",
  "pli-tv-pvr2.9:17.3_0": "Kaṁ ārabbhāti?",
  "pli-tv-pvr2.9:17.4_0": "Sundarīnandaṁ bhikkhuniṁ ārabbha.",
  "pli-tv-pvr2.9:17.5_0": "Kismiṁ vatthusminti?",
  "pli-tv-pvr2.9:17.6_0": "Sundarīnandā bhikkhunī avassutā avassutassa purisapuggalassa hatthato āmisaṁ paṭiggahesi,",
  "pli-tv-pvr2.9:17.6_1": "tasmiṁ vatthusmiṁ.",
  "pli-tv-pvr2.9:17.7_0": "Ekā paññatti.",
  "pli-tv-pvr2.9:17.8_0": "Channaṁ āpattisamuṭṭhānānaṁ ekena samuṭṭhānena samuṭṭhāti—",
  "pli-tv-pvr2.9:17.9_0": "paṭhamapārājike …pe….",
  "pli-tv-pvr2.9:18.1_0": "“Kiṁ te, ayye,",
  "pli-tv-pvr2.9:18.1_1": "eso purisapuggalo karissati avassuto vā anavassuto vā,",
  "pli-tv-pvr2.9:18.1_2": "yato tvaṁ anavassutā. Iṅgha, ayye,",
  "pli-tv-pvr2.9:18.1_3": "yaṁ te eso purisapuggalo deti khādanīyaṁ vā bhojanīyaṁ vā taṁ tvaṁ sahatthā paṭiggahetvā khāda vā bhuñja",
  "pli-tv-pvr2.9:18.1_4": "vā”ti uyyojanapaccayā saṅghādiseso kattha paññattoti?",
  "pli-tv-pvr2.9:18.2_0": "Sāvatthiyaṁ paññatto.",
  "pli-tv-pvr2.9:18.3_0": "Kaṁ ārabbhāti?",
  "pli-tv-pvr2.9:18.4_0": "Aññataraṁ bhikkhuniṁ ārabbha.",
  "pli-tv-pvr2.9:18.5_0": "Kismiṁ vatthusminti?",
  "pli-tv-pvr2.9:18.6_0": "Aññatarā bhikkhunī—“kiṁ te, ayye,",
  "pli-tv-pvr2.9:18.6_1": "eso purisapuggalo karissati avassuto vā anavassuto vā,",
  "pli-tv-pvr2.9:18.6_2": "yato tvaṁ anavassutā. Iṅgha, ayye,",
  "pli-tv-pvr2.9:18.6_3": "yaṁ te eso purisapuggalo deti khādanīyaṁ vā bhojanīyaṁ vā taṁ tvaṁ sahatthā paṭiggahetvā khāda vā bhuñja vā”ti uyyojesi,",
  "pli-tv-pvr2.9:18.6_4": "tasmiṁ vatthusmiṁ.",
  "pli-tv-pvr2.9:18.7_0": "Ekā paññatti.",
  "pli-tv-pvr2.9:18.8_0": "Channaṁ āpattisamuṭṭhānānaṁ tīhi samuṭṭhānehi samuṭṭhāti …pe….",
  "pli-tv-pvr2.9:19.1_0": "Kupitāya anattamanāya bhikkhuniyā yāvatatiyaṁ samanubhāsanāya na paṭinissajjanapaccayā saṅghādiseso kattha paññattoti?",
  "pli-tv-pvr2.9:19.2_0": "Sāvatthiyaṁ paññatto.",
  "pli-tv-pvr2.9:19.3_0": "Kaṁ ārabbhāti?",
  "pli-tv-pvr2.9:19.4_0": "Caṇḍakāḷiṁ bhikkhuniṁ ārabbha.",
  "pli-tv-pvr2.9:19.5_0": "Kismiṁ vatthusminti?",
  "pli-tv-pvr2.9:19.6_0": "Caṇḍakāḷī bhikkhunī kupitā anattamanā evaṁ avaca—“buddhaṁ paccācikkhāmi,",
  "pli-tv-pvr2.9:19.6_1": "dhammaṁ paccācikkhāmi,",
  "pli-tv-pvr2.9:19.6_2": "saṅghaṁ paccācikkhāmi,",
  "pli-tv-pvr2.9:19.6_3": "sikkhaṁ paccācikkhāmī”ti,",
  "pli-tv-pvr2.9:19.6_4": "tasmiṁ vatthusmiṁ.",
  "pli-tv-pvr2.9:19.7_0": "Ekā paññatti.",
  "pli-tv-pvr2.9:19.8_0": "Channaṁ āpattisamuṭṭhānānaṁ ekena samuṭṭhānena samuṭṭhāti—",
  "pli-tv-pvr2.9:19.9_0": "dhuranikkhepe …pe….",
  "pli-tv-pvr2.9:20.1_0": "Kismiñcideva adhikaraṇe paccākatāya bhikkhuniyā yāvatatiyaṁ samanubhāsanāya na paṭinissajjanapaccayā saṅghādiseso kattha paññattoti?",
  "pli-tv-pvr2.9:20.2_0": "Sāvatthiyaṁ paññatto.",
  "pli-tv-pvr2.9:20.3_0": "Kaṁ ārabbhāti?",
  "pli-tv-pvr2.9:20.4_0": "Caṇḍakāḷiṁ bhikkhuniṁ ārabbha.",
  "pli-tv-pvr2.9:20.5_0": "Kismiṁ vatthusminti?",
  "pli-tv-pvr2.9:20.6_0": "Caṇḍakāḷī bhikkhunī kismiñcideva adhikaraṇe paccākatā kupitā anattamanā evaṁ avaca—“chandagāminiyo ca bhikkhuniyo,",
  "pli-tv-pvr2.9:20.6_1": "dosagāminiyo ca bhikkhuniyo,",
  "pli-tv-pvr2.9:20.6_2": "mohagāminiyo ca bhikkhuniyo,",
  "pli-tv-pvr2.9:20.6_3": "bhayagāminiyo ca bhikkhuniyo”ti,",
  "pli-tv-pvr2.9:20.6_4": "tasmiṁ vatthusmiṁ.",
  "pli-tv-pvr2.9:20.7_0": "Ekā paññatti.",
  "pli-tv-pvr2.9:20.8_0": "Channaṁ āpattisamuṭṭhānānaṁ ekena samuṭṭhānena samuṭṭhāti—",
  "pli-tv-pvr2.9:20.9_0": "dhuranikkhepe …pe….",
  "pli-tv-pvr2.9:21.1_0": "Saṁsaṭṭhānaṁ bhikkhunīnaṁ yāvatatiyaṁ samanubhāsanāya na paṭinissajjanapaccayā saṅghādiseso kattha paññattoti?",
  "pli-tv-pvr2.9:21.2_0": "Sāvatthiyaṁ paññatto.",
  "pli-tv-pvr2.9:21.3_0": "Kaṁ ārabbhāti?",
  "pli-tv-pvr2.9:21.4_0": "Sambahulā bhikkhuniyo ārabbha.",
  "pli-tv-pvr2.9:21.5_0": "Kismiṁ vatthusminti?",
  "pli-tv-pvr2.9:21.6_0": "Sambahulā bhikkhuniyo saṁsaṭṭhā vihariṁsu, tasmiṁ vatthusmiṁ.",
  "pli-tv-pvr2.9:21.7_0": "Ekā paññatti.",
  "pli-tv-pvr2.9:21.8_0": "Channaṁ āpattisamuṭṭhānānaṁ ekena samuṭṭhānena samuṭṭhāti—",
  "pli-tv-pvr2.9:21.9_0": "dhuranikkhepe …pe….",
  "pli-tv-pvr2.9:22.1_0": "“Saṁsaṭṭhāva, ayye,",
  "pli-tv-pvr2.9:22.1_1": "tumhe viharatha.",
  "pli-tv-pvr2.9:22.1_2": "Mā tumhe nānā viharitthā”ti uyyojentiyā yāvatatiyaṁ samanubhāsanāya na paṭinissajjanapaccayā saṅghādiseso kattha paññattoti?",
  "pli-tv-pvr2.9:22.2_0": "Sāvatthiyaṁ paññatto.",
  "pli-tv-pvr2.9:22.3_0": "Kaṁ ārabbhāti?",
  "pli-tv-pvr2.9:22.4_0": "Thullanandaṁ bhikkhuniṁ ārabbha.",
  "pli-tv-pvr2.9:22.5_0": "Kismiṁ vatthusminti?",
  "pli-tv-pvr2.9:22.6_0": "Thullanandā bhikkhunī—“saṁsaṭṭhāva, ayye,",
  "pli-tv-pvr2.9:22.6_1": "tumhe viharatha.",
  "pli-tv-pvr2.9:22.6_2": "Mā tumhe nānā viharitthā”ti uyyojesi,",
  "pli-tv-pvr2.9:22.6_3": "tasmiṁ vatthusmiṁ.",
  "pli-tv-pvr2.9:22.7_0": "Ekā paññatti.",
  "pli-tv-pvr2.9:22.8_0": "Channaṁ āpattisamuṭṭhānānaṁ ekena samuṭṭhānena samuṭṭhāti—",
  "pli-tv-pvr2.9:22.9_0": "dhuranikkhepe …pe….",
  "pli-tv-pvr2.9:23.1_0": "Dadhiṁ viññāpetvā bhuñjanapaccayā pāṭidesanīyaṁ kattha paññattanti?",
  "pli-tv-pvr2.9:23.2_0": "Sāvatthiyaṁ paññattaṁ.",
  "pli-tv-pvr2.9:23.3_0": "Kaṁ ārabbhāti?",
  "pli-tv-pvr2.9:23.4_0": "Chabbaggiyā bhikkhuniyo ārabbha.",
  "pli-tv-pvr2.9:23.5_0": "Kismiṁ vatthusminti?",
  "pli-tv-pvr2.9:23.6_0": "Chabbaggiyā bhikkhuniyo dadhiṁ viññāpetvā bhuñjiṁsu, tasmiṁ vatthusmiṁ.",
  "pli-tv-pvr2.9:23.7_0": "Ekā paññatti, ekā anupaññatti.",
  "pli-tv-pvr2.9:23.8_0": "Channaṁ āpattisamuṭṭhānānaṁ catūhi samuṭṭhānehi samuṭṭhāti …pe….",
  "pli-tv-pvr2.9:23.9_0": "Katthapaññattivāro niṭṭhito paṭhamo."
}