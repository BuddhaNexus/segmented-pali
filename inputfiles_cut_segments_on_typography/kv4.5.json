{
  "kv4.5:0.1_0": "Kathāvatthu",
  "kv4.5:0.2_0": "Mahāpaṇṇāsaka",
  "kv4.5:0.3_0": "Catutthavagga",
  "kv4.5:0.4_0": "Upekkhāsamannāgatakathā",
  "kv4.5:1.1_0": "Arahā chahi upekkhāhi samannāgatoti?",
  "kv4.5:1.2_0": "Āmantā.",
  "kv4.5:1.3_0": "Arahā chahi phassehi, chahi vedanāhi, chahi saññāhi …pe… chahi paññāhi samannāgatoti?",
  "kv4.5:1.4_0": "Na hevaṁ vattabbe …pe….",
  "kv4.5:2.1_0": "Arahā chahi upekkhāhi samannāgatoti?",
  "kv4.5:2.2_0": "Āmantā.",
  "kv4.5:2.3_0": "Arahā cakkhunā rūpaṁ passanto sotena saddaṁ suṇāti,",
  "kv4.5:2.3_1": "ghānena gandhaṁ ghāyati,",
  "kv4.5:2.3_2": "jivhāya rasaṁ sāyati,",
  "kv4.5:2.3_3": "kāyena phoṭṭhabbaṁ phusati,",
  "kv4.5:2.3_4": "manasā dhammaṁ vijānāti …pe…",
  "kv4.5:2.4_0": "manasā dhammaṁ vijānanto cakkhunā rūpaṁ passati,",
  "kv4.5:2.4_1": "sotena saddaṁ suṇāti,",
  "kv4.5:2.4_2": "ghānena gandhaṁ ghāyati,",
  "kv4.5:2.4_3": "jivhāya rasaṁ sāyati,",
  "kv4.5:2.4_4": "kāyena phoṭṭhabbaṁ phusatīti?",
  "kv4.5:2.5_0": "Na hevaṁ vattabbe …pe….",
  "kv4.5:3.1_0": "Arahā chahi upekkhāhi samannāgatoti?",
  "kv4.5:3.2_0": "Āmantā.",
  "kv4.5:3.3_0": "Satataṁ samitaṁ abbokiṇṇaṁ chahi upekkhāhi samannāgato samohito, cha upekkhāyo paccupaṭṭhitāti?",
  "kv4.5:3.4_0": "Na hevaṁ vattabbe …pe….",
  "kv4.5:4.1_0": "Na vattabbaṁ—",
  "kv4.5:4.2_0": "“arahā chahi upekkhāhi samannāgato”ti?",
  "kv4.5:4.3_0": "Āmantā.",
  "kv4.5:4.4_0": "Nanu arahā chaḷaṅgupekkhoti?",
  "kv4.5:4.5_0": "Āmantā.",
  "kv4.5:4.6_0": "Hañci arahā chaḷaṅgupekkho, tena vata re vattabbe—",
  "kv4.5:4.7_0": "“arahā chahi upekkhāhi samannāgato”ti …pe….",
  "kv4.5:4.8_0": "Upekkhāsamannāgatakathā niṭṭhitā."
}