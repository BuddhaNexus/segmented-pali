{
  "vv61:0.1_0": "Vimānavatthu",
  "vv61:0.2_0": "Purisavimāna",
  "vv61:0.3_0": "Mahārathavagga",
  "vv61:0.4_0": "11. Dutiyanāgavimānavatthu",
  "vv61:1.1_0": "“Mahantaṁ nāgaṁ abhiruyha,",
  "vv61:1.2_0": "Sabbasetaṁ gajuttamaṁ;",
  "vv61:1.3_0": "Vanā vanaṁ anupariyāsi,",
  "vv61:1.4_0": "Nārīgaṇapurakkhato;",
  "vv61:1.5_0": "Obhāsento disā sabbā,",
  "vv61:1.6_0": "Osadhī viya tārakā.",
  "vv61:2.1_0": "Kena tetādiso vaṇṇo,",
  "vv61:2.2_0": "…pe…",
  "vv61:2.3_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv61:3.1_0": "So devaputto attamano,",
  "vv61:3.2_0": "vaṅgīseneva pucchito;",
  "vv61:3.3_0": "Pañhaṁ puṭṭho viyākāsi,",
  "vv61:3.4_0": "yassa kammassidaṁ phalaṁ.",
  "vv61:4.1_0": "“Ahaṁ manussesu manussabhūto,",
  "vv61:4.2_0": "Upāsako cakkhumato ahosiṁ;",
  "vv61:4.3_0": "Pāṇātipātā virato ahosiṁ,",
  "vv61:4.4_0": "Loke adinnaṁ parivajjayissaṁ.",
  "vv61:5.1_0": "Amajjapo no ca musā abhāṇiṁ,",
  "vv61:5.2_0": "Sakena dārena ca tuṭṭho ahosiṁ;",
  "vv61:5.3_0": "Annañca pānañca pasannacitto,",
  "vv61:5.4_0": "Sakkacca dānaṁ vipulaṁ adāsiṁ.",
  "vv61:6.1_0": "Tena metādiso vaṇṇo,",
  "vv61:6.2_0": "…pe…",
  "vv61:6.3_0": "vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv61:7.1_0": "Dutiyanāgavimānaṁ ekādasamaṁ."
}