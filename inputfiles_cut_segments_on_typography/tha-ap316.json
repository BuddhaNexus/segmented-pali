{
  "tha-ap316:0.1_0": "Therāpadāna",
  "tha-ap316:0.2_0": "Ārakkhadāyakavagga",
  "tha-ap316:0.3_0": "4. Sattapadumiyattheraapadāna",
  "tha-ap316:1.1_0": "“Nadīkūle vasāmahaṁ,",
  "tha-ap316:1.2_0": "nesādo nāma brāhmaṇo;",
  "tha-ap316:1.3_0": "Satapattehi pupphehi,",
  "tha-ap316:1.4_0": "sammajjitvāna assamaṁ.",
  "tha-ap316:2.1_0": "Suvaṇṇavaṇṇaṁ sambuddhaṁ,",
  "tha-ap316:2.2_0": "Siddhatthaṁ lokanāyakaṁ;",
  "tha-ap316:2.3_0": "Disvā nabhena gacchantaṁ,",
  "tha-ap316:2.4_0": "Hāso me udapajjatha.",
  "tha-ap316:3.1_0": "Paccuggantvāna sambuddhaṁ,",
  "tha-ap316:3.2_0": "lokajeṭṭhaṁ narāsabhaṁ;",
  "tha-ap316:3.3_0": "Assamaṁ atināmetvā,",
  "tha-ap316:3.4_0": "jalajaggehi okiriṁ.",
  "tha-ap316:4.1_0": "Catunnavutito kappe,",
  "tha-ap316:4.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap316:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap316:4.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap316:5.1_0": "Ito te sattame kappe,",
  "tha-ap316:5.2_0": "caturo pādapāvarā;",
  "tha-ap316:5.3_0": "Sattaratanasampannā,",
  "tha-ap316:5.4_0": "cakkavattī mahabbalā.",
  "tha-ap316:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap316:6.2_0": "…pe…",
  "tha-ap316:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap316:7.1_0": "Itthaṁ sudaṁ āyasmā sattapadumiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap316:8.1_0": "Sattapadumiyattherassāpadānaṁ catutthaṁ."
}