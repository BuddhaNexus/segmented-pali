{
  "ps2.8:0.1_0": "Paṭisambhidāmagga",
  "ps2.8:0.2_0": "2 Yuganaddhavagga",
  "ps2.8:0.3_0": "2.8. Lokuttarakathā",
  "ps2.8:1.1_0": "Katame dhammā lokuttarā?",
  "ps2.8:1.2_0": "Cattāro satipaṭṭhānā,",
  "ps2.8:1.2_1": "cattāro sammappadhānā,",
  "ps2.8:1.2_2": "cattāro iddhipādā,",
  "ps2.8:1.2_3": "pañcindriyāni,",
  "ps2.8:1.2_4": "pañca balāni,",
  "ps2.8:1.2_5": "satta bojjhaṅgā,",
  "ps2.8:1.2_6": "ariyo aṭṭhaṅgiko maggo,",
  "ps2.8:1.2_7": "cattāro ariyamaggā,",
  "ps2.8:1.2_8": "cattāri ca sāmaññaphalāni,",
  "ps2.8:1.2_9": "nibbānañca—",
  "ps2.8:1.3_0": "ime dhammā lokuttarā.",
  "ps2.8:2.1_0": "<b>Lokuttarā</b>ti kenaṭṭhena lokuttarā?",
  "ps2.8:2.2_0": "Lokaṁ tarantīti—lokuttarā.",
  "ps2.8:2.3_0": "Lokā uttarantīti—lokuttarā.",
  "ps2.8:2.4_0": "Lokato uttarantīti—lokuttarā.",
  "ps2.8:2.5_0": "Lokamhā uttarantīti—lokuttarā.",
  "ps2.8:2.6_0": "Lokaṁ atikkamantīti—lokuttarā.",
  "ps2.8:2.7_0": "Lokaṁ samatikkamantīti—lokuttarā.",
  "ps2.8:2.8_0": "Lokaṁ samatikkantāti—lokuttarā.",
  "ps2.8:2.9_0": "Lokena atirekāti—lokuttarā.",
  "ps2.8:2.10_0": "Lokantaṁ tarantīti—lokuttarā.",
  "ps2.8:2.11_0": "Lokā nissarantīti—lokuttarā.",
  "ps2.8:2.12_0": "Lokato nissarantīti—lokuttarā.",
  "ps2.8:2.13_0": "Lokamhā nissarantīti—lokuttarā.",
  "ps2.8:2.14_0": "Lokā nissaṭāti—lokuttarā.",
  "ps2.8:2.15_0": "Lokena nissaṭāti—lokuttarā.",
  "ps2.8:2.16_0": "Lokamhā nissaṭāti—lokuttarā.",
  "ps2.8:2.17_0": "Loke na tiṭṭhantīti—lokuttarā.",
  "ps2.8:2.18_0": "Lokasmiṁ na tiṭṭhantīti—lokuttarā.",
  "ps2.8:2.19_0": "Loke na limpantīti—lokuttarā.",
  "ps2.8:2.20_0": "Lokena na limpantīti—lokuttarā.",
  "ps2.8:2.21_0": "Loke asaṁlittāti—lokuttarā.",
  "ps2.8:2.22_0": "Lokena asaṁlittāti—lokuttarā.",
  "ps2.8:2.23_0": "Loke anupalittāti—lokuttarā.",
  "ps2.8:2.24_0": "Lokena anupalittāti—lokuttarā.",
  "ps2.8:2.25_0": "Loke vippamuttāti—lokuttarā.",
  "ps2.8:2.26_0": "Lokena vippamuttāti—lokuttarā.",
  "ps2.8:2.27_0": "Lokā vippamuttāti—lokuttarā.",
  "ps2.8:2.28_0": "Lokato vippamuttāti—lokuttarā.",
  "ps2.8:2.29_0": "Lokamhā vippamuttāti—lokuttarā.",
  "ps2.8:2.30_0": "Loke visaññuttāti—lokuttarā.",
  "ps2.8:2.31_0": "Lokena visaññuttāti—lokuttarā.",
  "ps2.8:2.32_0": "Lokā visaññuttāti—lokuttarā.",
  "ps2.8:2.33_0": "Lokasmiṁ visaññuttāti—lokuttarā.",
  "ps2.8:2.34_0": "Lokato visaññuttāti—lokuttarā.",
  "ps2.8:2.35_0": "Lokamhā visaññuttāti—lokuttarā.",
  "ps2.8:2.36_0": "Lokā sujjhantīti—lokuttarā.",
  "ps2.8:2.37_0": "Lokato sujjhantīti—lokuttarā.",
  "ps2.8:2.38_0": "Lokamhā sujjhantīti—lokuttarā.",
  "ps2.8:2.39_0": "Lokā visujjhantīti—lokuttarā.",
  "ps2.8:2.40_0": "Lokato visujjhantīti—lokuttarā.",
  "ps2.8:2.41_0": "Lokamhā visujjhantīti—lokuttarā.",
  "ps2.8:2.42_0": "Lokā vuṭṭhahantīti—lokuttarā.",
  "ps2.8:2.43_0": "Lokato vuṭṭhahantīti—lokuttarā.",
  "ps2.8:2.44_0": "Lokamhā vuṭṭhahantīti—lokuttarā.",
  "ps2.8:2.45_0": "Lokā vivaṭṭantīti—lokuttarā.",
  "ps2.8:2.46_0": "Lokato vivaṭṭantīti—lokuttarā.",
  "ps2.8:2.47_0": "Lokamhā vivaṭṭantīti—lokuttarā.",
  "ps2.8:2.48_0": "Loke na sajjantīti—lokuttarā.",
  "ps2.8:2.49_0": "Loke na gayhantīti—lokuttarā.",
  "ps2.8:2.50_0": "Loke na bajjhantīti—lokuttarā.",
  "ps2.8:2.51_0": "Lokaṁ samucchindantīti—lokuttarā.",
  "ps2.8:2.52_0": "Lokaṁ samucchinnattāti—lokuttarā.",
  "ps2.8:2.53_0": "Lokaṁ paṭippassambhentīti—lokuttarā.",
  "ps2.8:2.54_0": "Lokaṁ paṭippassambhitattāti—lokuttarā.",
  "ps2.8:2.55_0": "Lokassa apathāti—lokuttarā.",
  "ps2.8:2.56_0": "Lokassa agatīti—lokuttarā.",
  "ps2.8:2.57_0": "Lokassa avisayāti—lokuttarā.",
  "ps2.8:2.58_0": "Lokassa asādhāraṇāti—lokuttarā.",
  "ps2.8:2.59_0": "Lokaṁ vamantīti—lokuttarā.",
  "ps2.8:2.60_0": "Lokaṁ na paccāvamantīti—lokuttarā.",
  "ps2.8:2.61_0": "Lokaṁ pajahantīti—lokuttarā.",
  "ps2.8:2.62_0": "Lokaṁ na upādiyantīti—lokuttarā.",
  "ps2.8:2.63_0": "Lokaṁ visinentīti—lokuttarā.",
  "ps2.8:2.64_0": "Lokaṁ na ussinentīti—lokuttarā.",
  "ps2.8:2.65_0": "Lokaṁ vidhūpentīti—lokuttarā.",
  "ps2.8:2.66_0": "Lokaṁ na sandhūpentīti—lokuttarā.",
  "ps2.8:2.67_0": "Lokaṁ samatikkamma abhibhuyya tiṭṭhantīti—lokuttarā.",
  "ps2.8:2.68_0": "Lokuttarakathā niṭṭhitā."
}