{
  "ja502:0.1_0": "Jātaka",
  "ja502:0.2_0": "Vīsatinipāta",
  "ja502:0.3_0": "Mātaṅgavagga",
  "ja502:0.4_0": "6. Cūḷahaṁsajātaka",
  "ja502:1.1_0": "“Ete haṁsā pakkamanti,",
  "ja502:1.2_0": "vakkaṅgā bhayameritā;",
  "ja502:1.3_0": "Harittaca hemavaṇṇa,",
  "ja502:1.4_0": "kāmaṁ sumukha pakkama.",
  "ja502:2.1_0": "Ohāya maṁ ñātigaṇā,",
  "ja502:2.2_0": "ekaṁ pāsavasaṁ gataṁ;",
  "ja502:2.3_0": "Anapekkhamānā gacchanti,",
  "ja502:2.4_0": "kiṁ eko avahiyyasi.",
  "ja502:3.1_0": "Pateva patataṁ seṭṭha,",
  "ja502:3.2_0": "natthi baddhe sahāyatā;",
  "ja502:3.3_0": "Mā anīghāya hāpesi,",
  "ja502:3.4_0": "kāmaṁ sumukha pakkama”.",
  "ja502:4.1_0": "“Nāhaṁ ‘dukkhapareto’ti,",
  "ja502:4.2_0": "dhataraṭṭha tuvaṁ jahe;",
  "ja502:4.3_0": "Jīvitaṁ maraṇaṁ vā me,",
  "ja502:4.4_0": "tayā saddhiṁ bhavissati”.",
  "ja502:5.1_0": "“Etadariyassa kalyāṇaṁ,",
  "ja502:5.2_0": "yaṁ tvaṁ sumukha bhāsasi;",
  "ja502:5.3_0": "Tañca vīmaṁsamānohaṁ,",
  "ja502:5.4_0": "‘patatetaṁ’ avassajiṁ”.",
  "ja502:6.1_0": "“Apadena padaṁ yāti,",
  "ja502:6.2_0": "antalikkhacaro dijo;",
  "ja502:6.3_0": "Ārā pāsaṁ na bujjhi tvaṁ,",
  "ja502:6.4_0": "haṁsānaṁ pavaruttama”.",
  "ja502:7.1_0": "“Yadā parābhavo hoti,",
  "ja502:7.2_0": "poso jīvitasaṅkhaye;",
  "ja502:7.3_0": "Atha jālañca pāsañca,",
  "ja502:7.4_0": "āsajjāpi na bujjhati”.",
  "ja502:8.1_0": "“Ete haṁsā pakkamanti,",
  "ja502:8.2_0": "vakkaṅgā bhayameritā;",
  "ja502:8.3_0": "Harittaca hemavaṇṇa,",
  "ja502:8.4_0": "tvaññeva avahiyyasi.",
  "ja502:9.1_0": "Ete bhutvā ca pitvā ca,",
  "ja502:9.2_0": "pakkamanti vihaṅgamā;",
  "ja502:9.3_0": "Anapekkhamānā vakkaṅgā,",
  "ja502:9.4_0": "tvaññeveko upāsasi.",
  "ja502:10.1_0": "Kiṁ nu tyāyaṁ dijo hoti,",
  "ja502:10.2_0": "mutto baddhaṁ upāsasi;",
  "ja502:10.3_0": "Ohāya sakuṇā yanti,",
  "ja502:10.4_0": "kiṁ eko avahiyyasi”.",
  "ja502:11.1_0": "“Rājā me so dijo mitto,",
  "ja502:11.2_0": "sakhā pāṇasamo ca me;",
  "ja502:11.3_0": "Neva naṁ vijahissāmi,",
  "ja502:11.4_0": "yāva kālassa pariyāyaṁ”.",
  "ja502:12.1_0": "“Yo ca tvaṁ sakhino hetu,",
  "ja502:12.2_0": "pāṇaṁ cajitumicchasi;",
  "ja502:12.3_0": "So te sahāyaṁ muñcāmi,",
  "ja502:12.4_0": "hotu rājā tavānugo”.",
  "ja502:13.1_0": "“Evaṁ luddaka nandassu,",
  "ja502:13.2_0": "saha sabbehi ñātibhi;",
  "ja502:13.3_0": "Yathāhamajja nandāmi,",
  "ja502:13.4_0": "muttaṁ disvā dijādhipaṁ”.",
  "ja502:14.1_0": "“Kaccinnu bhoto kusalaṁ,",
  "ja502:14.2_0": "kacci bhoto anāmayaṁ;",
  "ja502:14.3_0": "Kacci raṭṭhamidaṁ phītaṁ,",
  "ja502:14.4_0": "dhammena manusāsasi”.",
  "ja502:15.1_0": "“Kusalañceva me haṁsa,",
  "ja502:15.2_0": "atho haṁsa anāmayaṁ;",
  "ja502:15.3_0": "Atho raṭṭhamidaṁ phītaṁ,",
  "ja502:15.4_0": "dhammena manusāsahaṁ”.",
  "ja502:16.1_0": "“Kacci bhoto amaccesu,",
  "ja502:16.2_0": "doso koci na vijjati;",
  "ja502:16.3_0": "Kacci ārā amittā te,",
  "ja502:16.4_0": "chāyā dakkhiṇatoriva”.",
  "ja502:17.1_0": "“Athopi me amaccesu,",
  "ja502:17.2_0": "doso koci na vijjati;",
  "ja502:17.3_0": "Atho ārā amittā me,",
  "ja502:17.4_0": "chāyā dakkhiṇatoriva”.",
  "ja502:18.1_0": "“Kacci te sādisī bhariyā,",
  "ja502:18.2_0": "assavā piyabhāṇinī;",
  "ja502:18.3_0": "Puttarūpayasūpetā,",
  "ja502:18.4_0": "tava chandavasānugā”.",
  "ja502:19.1_0": "“Atho me sādisī bhariyā,",
  "ja502:19.2_0": "assavā piyabhāṇinī;",
  "ja502:19.3_0": "Puttarūpayasūpetā,",
  "ja502:19.4_0": "mama chandavasānugā”.",
  "ja502:20.1_0": "“Kacci te bahavo puttā,",
  "ja502:20.2_0": "sujātā raṭṭhavaḍḍhana;",
  "ja502:20.3_0": "Paññājavena sampannā,",
  "ja502:20.4_0": "sammodanti tato tato”.",
  "ja502:21.1_0": "“Satameko ca me puttā,",
  "ja502:21.2_0": "dhataraṭṭha mayā sutā;",
  "ja502:21.3_0": "Tesaṁ tvaṁ kiccamakkhāhi,",
  "ja502:21.4_0": "nāvarujjhanti te vaco”.",
  "ja502:22.1_0": "“Upapannopi ce hoti,",
  "ja502:22.2_0": "jātiyā vinayena vā;",
  "ja502:22.3_0": "Atha pacchā kurute yogaṁ,",
  "ja502:22.4_0": "kicche āpāsu sīdati.",
  "ja502:23.1_0": "Tassa saṁhīrapaññassa,",
  "ja502:23.2_0": "vivaro jāyate mahā;",
  "ja502:23.3_0": "Rattimandhova rūpāni,",
  "ja502:23.4_0": "thūlāni manupassati.",
  "ja502:24.1_0": "Asāre sārayogaññū,",
  "ja502:24.2_0": "matiṁ na tveva vindati;",
  "ja502:24.3_0": "Sarabhova giriduggasmiṁ,",
  "ja502:24.4_0": "antarāyeva sīdati.",
  "ja502:25.1_0": "Hīnajaccopi ce hoti,",
  "ja502:25.2_0": "uṭṭhātā dhitimā naro;",
  "ja502:25.3_0": "Ācārasīlasampanno,",
  "ja502:25.4_0": "nise aggīva bhāsati.",
  "ja502:26.1_0": "Etaṁ me upamaṁ katvā,",
  "ja502:26.2_0": "putte vijjāsu vācaya;",
  "ja502:26.3_0": "Saṁvirūḷhetha medhāvī,",
  "ja502:26.4_0": "khette bījaṁva vuṭṭhiyā”ti.",
  "ja502:27.1_0": "Cūḷahaṁsajātakaṁ chaṭṭhaṁ."
}