{
  "thag1.29:0.1_0": "Theragāthā 1.29",
  "thag1.29:0.2_0": "Ekakanipāta",
  "thag1.29:0.3_0": "Tatiyavagga",
  "thag1.29:0.4_0": "Hāritattheragāthā",
  "thag1.29:1.1_0": "“Samunnamayamattānaṁ,",
  "thag1.29:1.2_0": "usukārova tejanaṁ;",
  "thag1.29:1.3_0": "Cittaṁ ujuṁ karitvāna,",
  "thag1.29:1.4_0": "avijjaṁ bhinda hāritā”ti.",
  "thag1.29:1.5_0": "… Hārito thero …."
}