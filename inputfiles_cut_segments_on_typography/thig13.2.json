{
  "thig13.2:0.1_0": "Therīgāthā 13.2",
  "thig13.2:0.2_0": "Vīsatinipāta",
  "thig13.2:0.3_0": "Rohinītherīgāthā",
  "thig13.2:1.1_0": "“‘Samaṇā’ti bhoti supi,",
  "thig13.2:1.2_0": "‘samaṇā’ti pabujjhasi;",
  "thig13.2:1.3_0": "Samaṇāneva kittesi,",
  "thig13.2:1.4_0": "samaṇī nūna bhavissasi.",
  "thig13.2:2.1_0": "Vipulaṁ annañca pānañca,",
  "thig13.2:2.2_0": "samaṇānaṁ paveccasi;",
  "thig13.2:2.3_0": "Rohinī dāni pucchāmi,",
  "thig13.2:2.4_0": "kena te samaṇā piyā.",
  "thig13.2:3.1_0": "Akammakāmā alasā,",
  "thig13.2:3.2_0": "paradattūpajīvino;",
  "thig13.2:3.3_0": "Āsaṁsukā sādukāmā,",
  "thig13.2:3.4_0": "kena te samaṇā piyā”.",
  "thig13.2:4.1_0": "“Cirassaṁ vata maṁ tāta,",
  "thig13.2:4.2_0": "samaṇānaṁ paripucchasi;",
  "thig13.2:4.3_0": "Tesaṁ te kittayissāmi,",
  "thig13.2:4.4_0": "paññāsīlaparakkamaṁ.",
  "thig13.2:5.1_0": "Kammakāmā analasā,",
  "thig13.2:5.2_0": "kammaseṭṭhassa kārakā;",
  "thig13.2:5.3_0": "Rāgaṁ dosaṁ pajahanti,",
  "thig13.2:5.4_0": "tena me samaṇā piyā.",
  "thig13.2:6.1_0": "Tīṇi pāpassa mūlāni,",
  "thig13.2:6.2_0": "dhunanti sucikārino;",
  "thig13.2:6.3_0": "Sabbaṁ pāpaṁ pahīnesaṁ,",
  "thig13.2:6.4_0": "tena me samaṇā piyā.",
  "thig13.2:7.1_0": "Kāyakammaṁ suci nesaṁ,",
  "thig13.2:7.2_0": "vacīkammañca tādisaṁ;",
  "thig13.2:7.3_0": "Manokammaṁ suci nesaṁ,",
  "thig13.2:7.4_0": "tena me samaṇā piyā.",
  "thig13.2:8.1_0": "Vimalā saṅkhamuttāva,",
  "thig13.2:8.2_0": "suddhā santarabāhirā;",
  "thig13.2:8.3_0": "Puṇṇā sukkāna dhammānaṁ,",
  "thig13.2:8.4_0": "tena me samaṇā piyā.",
  "thig13.2:9.1_0": "Bahussutā dhammadharā,",
  "thig13.2:9.2_0": "ariyā dhammajīvino;",
  "thig13.2:9.3_0": "Atthaṁ dhammañca desenti,",
  "thig13.2:9.4_0": "tena me samaṇā piyā.",
  "thig13.2:10.1_0": "Bahussutā dhammadharā,",
  "thig13.2:10.2_0": "ariyā dhammajīvino;",
  "thig13.2:10.3_0": "Ekaggacittā satimanto,",
  "thig13.2:10.4_0": "tena me samaṇā piyā.",
  "thig13.2:11.1_0": "Dūraṅgamā satimanto,",
  "thig13.2:11.2_0": "mantabhāṇī anuddhatā;",
  "thig13.2:11.3_0": "Dukkhassantaṁ pajānanti,",
  "thig13.2:11.4_0": "tena me samaṇā piyā.",
  "thig13.2:12.1_0": "Yasmā gāmā pakkamanti,",
  "thig13.2:12.2_0": "na vilokenti kiñcanaṁ;",
  "thig13.2:12.3_0": "Anapekkhāva gacchanti,",
  "thig13.2:12.4_0": "tena me samaṇā piyā.",
  "thig13.2:13.1_0": "Na te saṁ koṭṭhe openti,",
  "thig13.2:13.2_0": "na kumbhiṁ na khaḷopiyaṁ;",
  "thig13.2:13.3_0": "Pariniṭṭhitamesānā,",
  "thig13.2:13.4_0": "tena me samaṇā piyā.",
  "thig13.2:14.1_0": "Na te hiraññaṁ gaṇhanti,",
  "thig13.2:14.2_0": "na suvaṇṇaṁ na rūpiyaṁ;",
  "thig13.2:14.3_0": "Paccuppannena yāpenti,",
  "thig13.2:14.4_0": "tena me samaṇā piyā.",
  "thig13.2:15.1_0": "Nānākulā pabbajitā,",
  "thig13.2:15.2_0": "nānājanapadehi ca;",
  "thig13.2:15.3_0": "Aññamaññaṁ piyāyanti,",
  "thig13.2:15.4_0": "tena me samaṇā piyā”.",
  "thig13.2:16.1_0": "“Atthāya vata no bhoti,",
  "thig13.2:16.2_0": "kule jātāsi rohinī;",
  "thig13.2:16.3_0": "Saddhā buddhe ca dhamme ca,",
  "thig13.2:16.4_0": "saṅghe ca tibbagāravā.",
  "thig13.2:17.1_0": "Tuvaṁ hetaṁ pajānāsi,",
  "thig13.2:17.2_0": "puññakkhettaṁ anuttaraṁ;",
  "thig13.2:17.3_0": "Amhampi ete samaṇā,",
  "thig13.2:17.4_0": "paṭigaṇhanti dakkhiṇaṁ.",
  "thig13.2:18.1_0": "Patiṭṭhito hettha yañño,",
  "thig13.2:18.2_0": "vipulo no bhavissati”;",
  "thig13.2:18.3_0": "“Sace bhāyasi dukkhassa,",
  "thig13.2:18.4_0": "sace te dukkhamappiyaṁ.",
  "thig13.2:19.1_0": "Upehi saraṇaṁ buddhaṁ,",
  "thig13.2:19.2_0": "dhammaṁ saṅghañca tādinaṁ;",
  "thig13.2:19.3_0": "Samādiyāhi sīlāni,",
  "thig13.2:19.4_0": "taṁ te atthāya hehiti”.",
  "thig13.2:20.1_0": "“Upemi saraṇaṁ buddhaṁ,",
  "thig13.2:20.2_0": "dhammaṁ saṅghañca tādinaṁ;",
  "thig13.2:20.3_0": "Samādiyāmi sīlāni,",
  "thig13.2:20.4_0": "taṁ me atthāya hehiti.",
  "thig13.2:21.1_0": "Brahmabandhu pure āsiṁ,",
  "thig13.2:21.2_0": "so idānimhi brāhmaṇo;",
  "thig13.2:21.3_0": "Tevijjo sottiyo camhi,",
  "thig13.2:21.4_0": "vedagū camhi nhātako”.",
  "thig13.2:22.1_0": "… Rohinī therī …."
}