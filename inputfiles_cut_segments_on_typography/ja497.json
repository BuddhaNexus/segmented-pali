{
  "ja497:0.1_0": "Jātaka",
  "ja497:0.2_0": "Vīsatinipāta",
  "ja497:0.3_0": "Mātaṅgavagga",
  "ja497:0.4_0": "1. Mātaṅgajātaka",
  "ja497:1.1_0": "“Kuto nu āgacchasi dummavāsī,",
  "ja497:1.2_0": "Otallako paṁsupisācakova;",
  "ja497:1.3_0": "Saṅkāracoḷaṁ paṭimuñca kaṇṭhe,",
  "ja497:1.4_0": "Ko re tuvaṁ hosi adakkhiṇeyyo”.",
  "ja497:2.1_0": "“Annaṁ tavedaṁ pakataṁ yasassi,",
  "ja497:2.2_0": "Taṁ khajjare bhuñjare piyyare ca;",
  "ja497:2.3_0": "Jānāsi maṁ tvaṁ paradattūpajīviṁ,",
  "ja497:2.4_0": "Uttiṭṭhapiṇḍaṁ labhataṁ sapāko”.",
  "ja497:3.1_0": "“Annaṁ mamedaṁ pakataṁ brāhmaṇānaṁ,",
  "ja497:3.2_0": "Attatthāya saddahato mamedaṁ;",
  "ja497:3.3_0": "Apehi etto kimidhaṭṭhitosi,",
  "ja497:3.4_0": "Na mādisā tuyhaṁ dadanti jamma”.",
  "ja497:4.1_0": "“Thale ca ninne ca vapanti bījaṁ,",
  "ja497:4.2_0": "Anūpakhette phalamāsamānā;",
  "ja497:4.3_0": "Etāya saddhāya dadāhi dānaṁ,",
  "ja497:4.4_0": "Appeva ārādhaye dakkhiṇeyye”.",
  "ja497:5.1_0": "“Khettāni mayhaṁ viditāni loke,",
  "ja497:5.2_0": "Yesāhaṁ bījāni patiṭṭhapemi;",
  "ja497:5.3_0": "Ye brāhmaṇā jātimantūpapannā,",
  "ja497:5.4_0": "Tānīdha khettāni supesalāni”.",
  "ja497:6.1_0": "“Jātimado ca atimānitā ca,",
  "ja497:6.2_0": "Lobho ca doso ca mado ca moho;",
  "ja497:6.3_0": "Ete aguṇā yesu ca santi sabbe,",
  "ja497:6.4_0": "Tānīdha khettāni apesalāni.",
  "ja497:7.1_0": "Jātimado ca atimānitā ca,",
  "ja497:7.2_0": "Lobho ca doso ca mado ca moho;",
  "ja497:7.3_0": "Ete aguṇā yesu na santi sabbe,",
  "ja497:7.4_0": "Tānīdha khettāni supesalāni”.",
  "ja497:8.1_0": "“Kvettha gatā upajotiyo ca,",
  "ja497:8.2_0": "Upajjhāyo ca atha vā gaṇḍakucchi;",
  "ja497:8.3_0": "Imassa daṇḍañca vadhañca datvā,",
  "ja497:8.4_0": "Gale gahetvā khalayātha jammaṁ”.",
  "ja497:9.1_0": "“Giriṁ nakhena khaṇasi,",
  "ja497:9.2_0": "ayo dantehi khādasi;",
  "ja497:9.3_0": "Jātavedaṁ padahasi,",
  "ja497:9.4_0": "yo isiṁ paribhāsasi”.",
  "ja497:10.1_0": "“Idaṁ vatvāna mātaṅgo,",
  "ja497:10.2_0": "isi saccaparakkamo;",
  "ja497:10.3_0": "Antalikkhasmiṁ pakkāmi,",
  "ja497:10.4_0": "brāhmaṇānaṁ udikkhataṁ”.",
  "ja497:11.1_0": "“Āvellitaṁ piṭṭhito uttamaṅgaṁ,",
  "ja497:11.2_0": "Bāhuṁ pasāreti akammaneyyaṁ;",
  "ja497:11.3_0": "Setāni akkhīni yathā matassa,",
  "ja497:11.4_0": "Ko me imaṁ puttamakāsi evaṁ”.",
  "ja497:12.1_0": "“Idhāgamā samaṇo dummavāsī,",
  "ja497:12.2_0": "Otallako paṁsupisācakova;",
  "ja497:12.3_0": "Saṅkāracoḷaṁ paṭimuñca kaṇṭhe,",
  "ja497:12.4_0": "So te imaṁ puttamakāsi evaṁ”.",
  "ja497:13.1_0": "“Katamaṁ disaṁ agamā bhūripañño,",
  "ja497:13.2_0": "Akkhātha me māṇavā etamatthaṁ;",
  "ja497:13.3_0": "Gantvāna taṁ paṭikaremu accayaṁ,",
  "ja497:13.4_0": "Appeva naṁ putta labhemu jīvitaṁ”.",
  "ja497:14.1_0": "“Vehāyasaṁ agamā bhūripañño,",
  "ja497:14.2_0": "Pathaddhuno pannaraseva cando;",
  "ja497:14.3_0": "Api cāpi so purimadisaṁ agacchi,",
  "ja497:14.4_0": "Saccappaṭiñño isi sādhurūpo”.",
  "ja497:15.1_0": "“Āvellitaṁ piṭṭhito uttamaṅgaṁ,",
  "ja497:15.2_0": "Bāhuṁ pasāreti akammaneyyaṁ;",
  "ja497:15.3_0": "Setāni akkhīni yathā matassa,",
  "ja497:15.4_0": "Ko me imaṁ puttamakāsi evaṁ”.",
  "ja497:16.1_0": "“Yakkhā have santi mahānubhāvā,",
  "ja497:16.2_0": "Anvāgatā isayo sādhurūpā;",
  "ja497:16.3_0": "Te duṭṭhacittaṁ kupitaṁ viditvā,",
  "ja497:16.4_0": "Yakkhā hi te puttamakaṁsu evaṁ”.",
  "ja497:17.1_0": "“Yakkhā ca me puttamakaṁsu evaṁ,",
  "ja497:17.2_0": "Tvaññeva me mā kuddho brahmacāri;",
  "ja497:17.3_0": "Tumheva pāde saraṇaṁ gatāsmi,",
  "ja497:17.4_0": "Anvāgatā puttasokena bhikkhu”.",
  "ja497:18.1_0": "“Tadeva hi etarahi ca mayhaṁ,",
  "ja497:18.2_0": "Manopadoso na mamatthi koci;",
  "ja497:18.3_0": "Putto ca te vedamadena matto,",
  "ja497:18.4_0": "Atthaṁ na jānāti adhicca vede”.",
  "ja497:19.1_0": "“Addhā have bhikkhu muhuttakena,",
  "ja497:19.2_0": "Sammuyhateva purisassa saññā;",
  "ja497:19.3_0": "Ekāparādhaṁ khama bhūripañña,",
  "ja497:19.4_0": "Na paṇḍitā kodhabalā bhavanti”.",
  "ja497:20.1_0": "“Idañca mayhaṁ uttiṭṭhapiṇḍaṁ,",
  "ja497:20.2_0": "Tava maṇḍabyo bhuñjatu appapañño;",
  "ja497:20.3_0": "Yakkhā ca te naṁ na viheṭhayeyyuṁ,",
  "ja497:20.4_0": "Putto ca te hessati so arogo”.",
  "ja497:21.1_0": "“Maṇḍabya bālosi parittapañño,",
  "ja497:21.2_0": "Yo puññakhettānamakovidosi;",
  "ja497:21.3_0": "Mahakkasāvesu dadāsi dānaṁ,",
  "ja497:21.4_0": "Kiliṭṭhakammesu asaññatesu.",
  "ja497:22.1_0": "Jaṭā ca kesā ajinā nivatthā,",
  "ja497:22.2_0": "Jarūdapānaṁva mukhaṁ parūḷhaṁ;",
  "ja497:22.3_0": "Pajaṁ imaṁ passatha dummarūpaṁ,",
  "ja497:22.4_0": "Na jaṭājinaṁ tāyati appapaññaṁ.",
  "ja497:23.1_0": "Yesaṁ rāgo ca doso ca,",
  "ja497:23.2_0": "avijjā ca virājitā;",
  "ja497:23.3_0": "Khīṇāsavā arahanto,",
  "ja497:23.4_0": "tesu dinnaṁ mahapphalan”ti.",
  "ja497:24.1_0": "Mātaṅgajātakaṁ paṭhamaṁ."
}