{
  "kv5.8:0.1_0": "Kathāvatthu",
  "kv5.8:0.2_0": "Mahāpaṇṇāsaka",
  "kv5.8:0.3_0": "Pañcamavagga",
  "kv5.8:0.4_0": "Anāgatañāṇakathā",
  "kv5.8:1.1_0": "Anāgate ñāṇaṁ atthīti?",
  "kv5.8:1.2_0": "Āmantā.",
  "kv5.8:1.3_0": "Anāgataṁ mūlato jānāti,",
  "kv5.8:1.3_1": "hetuto jānāti,",
  "kv5.8:1.3_2": "nidānato jānāti,",
  "kv5.8:1.3_3": "sambhavato jānāti,",
  "kv5.8:1.3_4": "pabhavato jānāti,",
  "kv5.8:1.3_5": "samuṭṭhānato jānāti,",
  "kv5.8:1.3_6": "āhārato jānāti,",
  "kv5.8:1.3_7": "ārammaṇato jānāti,",
  "kv5.8:1.3_8": "paccayato jānāti,",
  "kv5.8:1.3_9": "samudayato jānātīti?",
  "kv5.8:1.4_0": "Na hevaṁ vattabbe …pe….",
  "kv5.8:2.1_0": "Anāgate ñāṇaṁ atthīti?",
  "kv5.8:2.2_0": "Āmantā.",
  "kv5.8:2.3_0": "Anāgataṁ hetupaccayataṁ jānāti,",
  "kv5.8:2.3_1": "ārammaṇapaccayataṁ jānāti,",
  "kv5.8:2.3_2": "adhipatipaccayataṁ jānāti,",
  "kv5.8:2.3_3": "anantarapaccayataṁ jānāti,",
  "kv5.8:2.3_4": "samanantarapaccayataṁ jānātīti?",
  "kv5.8:2.4_0": "Na hevaṁ vattabbe …pe….",
  "kv5.8:3.1_0": "Anāgate ñāṇaṁ atthīti?",
  "kv5.8:3.2_0": "Āmantā.",
  "kv5.8:3.3_0": "Gotrabhuno puggalassa sotāpattimagge ñāṇaṁ atthīti?",
  "kv5.8:3.4_0": "Na hevaṁ vattabbe …pe….",
  "kv5.8:3.5_0": "Sotāpattiphalasacchikiriyāya paṭipannassa puggalassa sotāpattiphale ñāṇaṁ atthīti?",
  "kv5.8:3.6_0": "Na hevaṁ vattabbe …pe….",
  "kv5.8:4.1_0": "Sakadāgāmi …pe…",
  "kv5.8:4.2_0": "anāgāmi …pe…",
  "kv5.8:4.3_0": "arahattasacchikiriyāya paṭipannassa puggalassa arahatte ñāṇaṁ atthīti?",
  "kv5.8:4.4_0": "Na hevaṁ vattabbe …pe….",
  "kv5.8:5.1_0": "Na vattabbaṁ—",
  "kv5.8:5.2_0": "“anāgate ñāṇaṁ atthī”ti?",
  "kv5.8:5.3_0": "Āmantā.",
  "kv5.8:5.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv5.8:5.5_0": "“pāṭaliputtassa kho, ānanda, tayo antarāyā bhavissanti—",
  "kv5.8:5.6_0": "aggito vā udakato vā mithubhedā vā”ti.",
  "kv5.8:5.7_0": "Attheva suttantoti?",
  "kv5.8:5.8_0": "Āmantā.",
  "kv5.8:5.9_0": "Tena hi anāgate ñāṇaṁ atthīti.",
  "kv5.8:5.10_0": "Anāgatañāṇakathā niṭṭhitā."
}