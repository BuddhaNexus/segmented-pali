{
  "pli-tv-pvr3:0.1_0": "Parivāra",
  "pli-tv-pvr3:0.2_0": "3. Samuṭṭhānasīsasaṅkhepa",
  "pli-tv-pvr3:0.3_0": "1. Samuṭṭhānassuddāna",
  "pli-tv-pvr3:1.1_0": "Aniccā sabbe saṅkhārā,",
  "pli-tv-pvr3:1.2_0": "dukkhānattā ca saṅkhatā;",
  "pli-tv-pvr3:1.3_0": "Nibbānañceva paññatti,",
  "pli-tv-pvr3:1.4_0": "anattā iti nicchayā.",
  "pli-tv-pvr3:2.1_0": "Buddhacande anuppanne,",
  "pli-tv-pvr3:2.2_0": "buddhādicce anuggate;",
  "pli-tv-pvr3:2.3_0": "Tesaṁ sabhāgadhammānaṁ,",
  "pli-tv-pvr3:2.4_0": "nāmamattaṁ na nāyati.",
  "pli-tv-pvr3:3.1_0": "Dukkaraṁ vividhaṁ katvā,",
  "pli-tv-pvr3:3.2_0": "pūrayitvāna pāramī;",
  "pli-tv-pvr3:3.3_0": "Uppajjanti mahāvīrā,",
  "pli-tv-pvr3:3.4_0": "cakkhubhūtā sabrahmake.",
  "pli-tv-pvr3:4.1_0": "Te desayanti saddhammaṁ,",
  "pli-tv-pvr3:4.2_0": "dukkhahāniṁ sukhāvahaṁ;",
  "pli-tv-pvr3:4.3_0": "Aṅgīraso sakyamuni,",
  "pli-tv-pvr3:4.4_0": "sabbabhūtānukampako.",
  "pli-tv-pvr3:5.1_0": "Sabbasattuttamo sīho,",
  "pli-tv-pvr3:5.2_0": "piṭake tīṇi desayi;",
  "pli-tv-pvr3:5.3_0": "Suttantamabhidhammañca,",
  "pli-tv-pvr3:5.4_0": "vinayañca mahāguṇaṁ.",
  "pli-tv-pvr3:6.1_0": "Evaṁ nīyati saddhammo,",
  "pli-tv-pvr3:6.2_0": "vinayo yadi tiṭṭhati;",
  "pli-tv-pvr3:6.3_0": "Ubhato ca vibhaṅgāni,",
  "pli-tv-pvr3:6.4_0": "khandhakā yā ca mātikā.",
  "pli-tv-pvr3:7.1_0": "Mālā suttaguṇeneva,",
  "pli-tv-pvr3:7.2_0": "parivārena ganthitā;",
  "pli-tv-pvr3:7.3_0": "Tasseva parivārassa,",
  "pli-tv-pvr3:7.4_0": "samuṭṭhānaṁ niyato kataṁ.",
  "pli-tv-pvr3:8.1_0": "Sambhedaṁ nidānañcaññaṁ,",
  "pli-tv-pvr3:8.2_0": "sutte dissanti upari;",
  "pli-tv-pvr3:8.3_0": "Tasmā sikkhe parivāraṁ,",
  "pli-tv-pvr3:8.4_0": "dhammakāmo supesaloti.",
  "pli-tv-pvr3:9.0_0": "2. Terasasamuṭṭhāna",
  "pli-tv-pvr3:9.1_0": "Vibhaṅge dvīsu paññattaṁ,",
  "pli-tv-pvr3:9.2_0": "uddisanti uposathe;",
  "pli-tv-pvr3:9.3_0": "Pavakkhāmi samuṭṭhānaṁ,",
  "pli-tv-pvr3:9.4_0": "yathāñāyaṁ suṇātha me.",
  "pli-tv-pvr3:10.1_0": "Pārājikaṁ yaṁ paṭhamaṁ,",
  "pli-tv-pvr3:10.2_0": "dutiyañca tato paraṁ;",
  "pli-tv-pvr3:10.3_0": "Sañcarittānubhāsanañca,",
  "pli-tv-pvr3:10.4_0": "atirekañca cīvaraṁ.",
  "pli-tv-pvr3:11.1_0": "Lomāni padasodhammo,",
  "pli-tv-pvr3:11.2_0": "bhūtaṁ saṁvidhānena ca;",
  "pli-tv-pvr3:11.3_0": "Theyyadesanacorī ca,",
  "pli-tv-pvr3:11.4_0": "ananuññātāya terasa.",
  "pli-tv-pvr3:12.1_0": "Terasete samuṭṭhāna-",
  "pli-tv-pvr3:12.2_0": "nayā viññūhi cintitā;",
  "pli-tv-pvr3:12.3_0": "Ekekasmiṁ samuṭṭhāne,",
  "pli-tv-pvr3:12.4_0": "sadisā idha dissare.",
  "pli-tv-pvr3:13.0_0": "2.1. Paṭhamapārājikasamuṭṭhāna",
  "pli-tv-pvr3:13.1_0": "Methunaṁ sukkasaṁsaggo,",
  "pli-tv-pvr3:13.2_0": "aniyatā paṭhamikā;",
  "pli-tv-pvr3:13.3_0": "Pubbūpaparipācitā,",
  "pli-tv-pvr3:13.4_0": "raho bhikkhuniyā saha.",
  "pli-tv-pvr3:14.1_0": "Sabhojane raho dve ca,",
  "pli-tv-pvr3:14.2_0": "aṅguli udake hasaṁ;",
  "pli-tv-pvr3:14.3_0": "Pahāre uggire ceva,",
  "pli-tv-pvr3:14.4_0": "tepaññāsā ca sekhiyā.",
  "pli-tv-pvr3:15.1_0": "Adhakkhagāmāvassutā,",
  "pli-tv-pvr3:15.2_0": "talamaṭṭhañca suddhikā;",
  "pli-tv-pvr3:15.3_0": "Vassaṁvuṭṭhā ca ovādaṁ,",
  "pli-tv-pvr3:15.4_0": "nānubandhe pavattiniṁ.",
  "pli-tv-pvr3:16.1_0": "Chasattati ime sikkhā,",
  "pli-tv-pvr3:16.2_0": "kāyamānasikā katā;",
  "pli-tv-pvr3:16.3_0": "Sabbe ekasamuṭṭhānā,",
  "pli-tv-pvr3:16.4_0": "paṭhamaṁ pārājikaṁ yathā.",
  "pli-tv-pvr3:17.1_0": "Paṭhamapārājikasamuṭṭhānaṁ niṭṭhitaṁ.",
  "pli-tv-pvr3:18.0_0": "2.2. Dutiyapārājikasamuṭṭhāna",
  "pli-tv-pvr3:18.1_0": "Adinnaṁ viggahuttari,",
  "pli-tv-pvr3:18.2_0": "duṭṭhullā attakāminaṁ;",
  "pli-tv-pvr3:18.3_0": "Amūlā aññabhāgiyā,",
  "pli-tv-pvr3:18.4_0": "aniyatā dutiyikā.",
  "pli-tv-pvr3:19.1_0": "Acchinde pariṇāmane,",
  "pli-tv-pvr3:19.2_0": "musā omasapesuṇā;",
  "pli-tv-pvr3:19.3_0": "Duṭṭhullā pathavīkhaṇe,",
  "pli-tv-pvr3:19.4_0": "bhūtaṁ aññāya ujjhāpe.",
  "pli-tv-pvr3:20.1_0": "Nikkaḍḍhanaṁ siñcanañca,",
  "pli-tv-pvr3:20.2_0": "āmisahetu bhuttāvī;",
  "pli-tv-pvr3:20.3_0": "Ehi anādari bhiṁsā,",
  "pli-tv-pvr3:20.4_0": "apanidhe ca jīvitaṁ.",
  "pli-tv-pvr3:21.1_0": "Jānaṁ sappāṇakaṁ kammaṁ,",
  "pli-tv-pvr3:21.2_0": "ūnasaṁvāsanāsanā;",
  "pli-tv-pvr3:21.3_0": "Sahadhammikavilekhā,",
  "pli-tv-pvr3:21.4_0": "moho amūlakena ca.",
  "pli-tv-pvr3:22.1_0": "Kukkuccaṁ dhammikaṁ cīvaraṁ datvā,",
  "pli-tv-pvr3:22.2_0": "Pariṇāmeyya puggale;",
  "pli-tv-pvr3:22.3_0": "Kiṁ te akālaṁ acchinde,",
  "pli-tv-pvr3:22.4_0": "Duggahī nirayena ca.",
  "pli-tv-pvr3:23.1_0": "Gaṇaṁ vibhaṅgaṁ dubbalaṁ,",
  "pli-tv-pvr3:23.2_0": "kathināphāsupassayaṁ;",
  "pli-tv-pvr3:23.3_0": "Akkosacaṇḍī maccharī,",
  "pli-tv-pvr3:23.4_0": "gabbhinī ca pāyantiyā.",
  "pli-tv-pvr3:24.1_0": "Dvevassaṁ sikkhā saṅghena,",
  "pli-tv-pvr3:24.2_0": "tayo ceva gihīgatā;",
  "pli-tv-pvr3:24.3_0": "Kumāribhūtā tisso ca,",
  "pli-tv-pvr3:24.4_0": "ūnadvādasasammatā.",
  "pli-tv-pvr3:25.1_0": "Alaṁ tāva sokāvāsaṁ,",
  "pli-tv-pvr3:25.2_0": "chandā anuvassā ca dve;",
  "pli-tv-pvr3:25.3_0": "Sikkhāpadā sattatime,",
  "pli-tv-pvr3:25.4_0": "samuṭṭhānā tikā katā.",
  "pli-tv-pvr3:26.1_0": "Kāyacittena na vācā,",
  "pli-tv-pvr3:26.2_0": "vācācittaṁ na kāyikaṁ;",
  "pli-tv-pvr3:26.3_0": "Tīhi dvārehi jāyanti,",
  "pli-tv-pvr3:26.4_0": "pārājikaṁ dutiyaṁ yathā.",
  "pli-tv-pvr3:27.1_0": "Dutiyapārājikasamuṭṭhānaṁ niṭṭhitaṁ.",
  "pli-tv-pvr3:28.0_0": "2.3. Sañcarittasamuṭṭhāna",
  "pli-tv-pvr3:28.1_0": "Sañcarī kuṭi vihāro,",
  "pli-tv-pvr3:28.2_0": "dhovanañca paṭiggaho;",
  "pli-tv-pvr3:28.3_0": "Viññattuttari abhihaṭṭhuṁ,",
  "pli-tv-pvr3:28.4_0": "ubhinnaṁ dūtakena ca.",
  "pli-tv-pvr3:29.1_0": "Kosiyā suddhadvebhāgā,",
  "pli-tv-pvr3:29.2_0": "chabbassāni nisīdanaṁ;",
  "pli-tv-pvr3:29.3_0": "Riñcanti rūpikā ceva,",
  "pli-tv-pvr3:29.4_0": "ubho nānappakārakā.",
  "pli-tv-pvr3:30.1_0": "Ūnabandhanavassikā,",
  "pli-tv-pvr3:30.2_0": "suttaṁ vikappanena ca;",
  "pli-tv-pvr3:30.3_0": "Dvāradānasibbāni ca,",
  "pli-tv-pvr3:30.4_0": "pūvapaccayajoti ca.",
  "pli-tv-pvr3:31.1_0": "Ratanaṁ sūci mañco ca,",
  "pli-tv-pvr3:31.2_0": "tūlaṁ nisīdanakaṇḍu ca;",
  "pli-tv-pvr3:31.3_0": "Vassikā ca sugatena,",
  "pli-tv-pvr3:31.4_0": "viññatti aññaṁ cetāpanā.",
  "pli-tv-pvr3:32.1_0": "Dve saṅghikā mahājanikā dve,",
  "pli-tv-pvr3:32.2_0": "Puggalalahukā garu;",
  "pli-tv-pvr3:32.3_0": "Dve vighāsā sāṭikā ca,",
  "pli-tv-pvr3:32.4_0": "Samaṇacīvarena ca.",
  "pli-tv-pvr3:33.1_0": "Samapaññāsime dhammā,",
  "pli-tv-pvr3:33.2_0": "chahi ṭhānehi jāyare;",
  "pli-tv-pvr3:33.3_0": "Kāyato na vācācittā,",
  "pli-tv-pvr3:33.4_0": "vācato na kāyamanā.",
  "pli-tv-pvr3:34.1_0": "Kāyavācā na ca cittā,",
  "pli-tv-pvr3:34.2_0": "kāyacittā na vācikā;",
  "pli-tv-pvr3:34.3_0": "Vācācittā na kāyena,",
  "pli-tv-pvr3:34.4_0": "tīhi dvārehi jāyare;",
  "pli-tv-pvr3:34.5_0": "Chasamuṭṭhānikā ete,",
  "pli-tv-pvr3:34.6_0": "sañcarittena sādisā.",
  "pli-tv-pvr3:35.1_0": "Sañcarittasamuṭṭhānaṁ niṭṭhitaṁ.",
  "pli-tv-pvr3:36.0_0": "2.4. Samanubhāsanāsamuṭṭhāna",
  "pli-tv-pvr3:36.1_0": "Bhedānuvattadubbaca-",
  "pli-tv-pvr3:36.2_0": "dūsaduṭṭhulladiṭṭhi ca;",
  "pli-tv-pvr3:36.3_0": "Chandaṁ ujjagghikā dve ca,",
  "pli-tv-pvr3:36.4_0": "dve ca saddā na byāhare.",
  "pli-tv-pvr3:37.1_0": "Chamā nīcāsane ṭhānaṁ,",
  "pli-tv-pvr3:37.2_0": "pacchato uppathena ca;",
  "pli-tv-pvr3:37.3_0": "Vajjānuvattigahaṇā,",
  "pli-tv-pvr3:37.4_0": "osāre paccācikkhanā.",
  "pli-tv-pvr3:38.1_0": "Kismiṁ saṁsaṭṭhā dve vadhi,",
  "pli-tv-pvr3:38.2_0": "Visibbe dukkhitāya ca;",
  "pli-tv-pvr3:38.3_0": "Puna saṁsaṭṭhā na vūpasame,",
  "pli-tv-pvr3:38.4_0": "Ārāmañca pavāraṇā.",
  "pli-tv-pvr3:39.1_0": "Anvaddhaṁ saha jīviniṁ dve,",
  "pli-tv-pvr3:39.2_0": "Cīvaraṁ anubandhanā;",
  "pli-tv-pvr3:39.3_0": "Sattatiṁsa ime dhammā,",
  "pli-tv-pvr3:39.4_0": "Kāyavācāya cittato;",
  "pli-tv-pvr3:39.5_0": "Sabbe ekasamuṭṭhānā,",
  "pli-tv-pvr3:39.6_0": "Samanubhāsanā yathā.",
  "pli-tv-pvr3:40.1_0": "Samanubhāsanāsamuṭṭhānaṁ niṭṭhitaṁ.",
  "pli-tv-pvr3:41.0_0": "2.5. Kathinasamuṭṭhāna",
  "pli-tv-pvr3:41.1_0": "Ubbhataṁ kathinaṁ tīṇi,",
  "pli-tv-pvr3:41.2_0": "paṭhamaṁ pattabhesajjaṁ;",
  "pli-tv-pvr3:41.3_0": "Accekaṁ cāpi sāsaṅkaṁ,",
  "pli-tv-pvr3:41.4_0": "pakkamantena vā duve.",
  "pli-tv-pvr3:42.1_0": "Upassayaṁ paramparā,",
  "pli-tv-pvr3:42.2_0": "anatirittaṁ nimantanā;",
  "pli-tv-pvr3:42.3_0": "Vikappaṁ rañño vikāle,",
  "pli-tv-pvr3:42.4_0": "vosāsāraññakena ca.",
  "pli-tv-pvr3:43.1_0": "Ussayāsannicayañca,",
  "pli-tv-pvr3:43.2_0": "pure pacchā vikāle ca;",
  "pli-tv-pvr3:43.3_0": "Pañcāhikā saṅkamanī,",
  "pli-tv-pvr3:43.4_0": "dvepi āvasathena ca.",
  "pli-tv-pvr3:44.1_0": "Pasākhe āsane ceva,",
  "pli-tv-pvr3:44.2_0": "tiṁsa ekūnakā ime;",
  "pli-tv-pvr3:44.3_0": "Kāyavācā na ca cittā,",
  "pli-tv-pvr3:44.4_0": "tīhi dvārehi jāyare;",
  "pli-tv-pvr3:44.5_0": "Dvisamuṭṭhānikā sabbe,",
  "pli-tv-pvr3:44.6_0": "kathinena sahā samā.",
  "pli-tv-pvr3:45.1_0": "Kathinasamuṭṭhānaṁ niṭṭhitaṁ.",
  "pli-tv-pvr3:46.0_0": "2.6. Eḷakalomasamuṭṭhāna",
  "pli-tv-pvr3:46.1_0": "Eḷakalomā dve seyyā,",
  "pli-tv-pvr3:46.2_0": "āhacca piṇḍabhojanaṁ;",
  "pli-tv-pvr3:46.3_0": "Gaṇavikālasannidhi,",
  "pli-tv-pvr3:46.4_0": "dantaponena celakā.",
  "pli-tv-pvr3:47.1_0": "Uyyuttaṁ senaṁ uyyodhi,",
  "pli-tv-pvr3:47.2_0": "surā orena nhāyanā;",
  "pli-tv-pvr3:47.3_0": "Dubbaṇṇe dve desanikā,",
  "pli-tv-pvr3:47.4_0": "lasuṇupatiṭṭhe naccanā.",
  "pli-tv-pvr3:48.1_0": "Nhānamattharaṇaṁ seyyā,",
  "pli-tv-pvr3:48.2_0": "antoraṭṭhe tathā bahi;",
  "pli-tv-pvr3:48.3_0": "Antovassaṁ cittāgāraṁ,",
  "pli-tv-pvr3:48.4_0": "āsandi suttakantanā.",
  "pli-tv-pvr3:49.1_0": "Veyyāvaccaṁ sahatthā ca,",
  "pli-tv-pvr3:49.2_0": "abhikkhukāvāsena ca;",
  "pli-tv-pvr3:49.3_0": "Chattaṁ yānañca saṅghāṇiṁ,",
  "pli-tv-pvr3:49.4_0": "alaṅkāraṁ gandhavāsitaṁ.",
  "pli-tv-pvr3:50.1_0": "Bhikkhunī sikkhamānā ca,",
  "pli-tv-pvr3:50.2_0": "sāmaṇerī gihiniyā;",
  "pli-tv-pvr3:50.3_0": "Asaṅkaccikā āpatti,",
  "pli-tv-pvr3:50.4_0": "cattārīsā catuttari.",
  "pli-tv-pvr3:51.1_0": "Kāyena na vācācittena,",
  "pli-tv-pvr3:51.2_0": "kāyacittena na vācato;",
  "pli-tv-pvr3:51.3_0": "Dvisamuṭṭhānikā sabbe,",
  "pli-tv-pvr3:51.4_0": "samā eḷakalomikāti.",
  "pli-tv-pvr3:52.1_0": "Eḷakalomasamuṭṭhānaṁ niṭṭhitaṁ.",
  "pli-tv-pvr3:53.0_0": "2.7. Padasodhammasamuṭṭhāna",
  "pli-tv-pvr3:53.1_0": "Padaññatra asammatā,",
  "pli-tv-pvr3:53.2_0": "tathā atthaṅgatena ca;",
  "pli-tv-pvr3:53.3_0": "Tiracchānavijjā dve vuttā,",
  "pli-tv-pvr3:53.4_0": "anokāso ca pucchanā.",
  "pli-tv-pvr3:54.1_0": "Satta sikkhāpadā ete,",
  "pli-tv-pvr3:54.2_0": "vācā na kāyacittato;",
  "pli-tv-pvr3:54.3_0": "Vācācittena jāyanti,",
  "pli-tv-pvr3:54.4_0": "na tu kāyena jāyare;",
  "pli-tv-pvr3:54.5_0": "Dvisamuṭṭhānikā sabbe,",
  "pli-tv-pvr3:54.6_0": "padasodhammasadisā.",
  "pli-tv-pvr3:55.1_0": "Padasodhammasamuṭṭhānaṁ niṭṭhitaṁ.",
  "pli-tv-pvr3:56.0_0": "2.8. Addhānasamuṭṭhāna",
  "pli-tv-pvr3:56.1_0": "Addhānanāvaṁ paṇītaṁ,",
  "pli-tv-pvr3:56.2_0": "mātugāmena saṁhare;",
  "pli-tv-pvr3:56.3_0": "Dhaññaṁ nimantitā ceva,",
  "pli-tv-pvr3:56.4_0": "aṭṭha ca pāṭidesanī.",
  "pli-tv-pvr3:57.1_0": "Sikkhā pannarasa ete,",
  "pli-tv-pvr3:57.2_0": "kāyā na vācā na manā;",
  "pli-tv-pvr3:57.3_0": "Kāyavācāhi jāyanti,",
  "pli-tv-pvr3:57.4_0": "na te cittena jāyare.",
  "pli-tv-pvr3:58.1_0": "Kāyacittena jāyanti,",
  "pli-tv-pvr3:58.2_0": "na te jāyanti vācato;",
  "pli-tv-pvr3:58.3_0": "Kāyavācāhi cittena,",
  "pli-tv-pvr3:58.4_0": "samuṭṭhānā catubbidhā;",
  "pli-tv-pvr3:58.5_0": "Paññattā buddhañāṇena,",
  "pli-tv-pvr3:58.6_0": "addhānena sahā samā.",
  "pli-tv-pvr3:59.1_0": "Addhānasamuṭṭhānaṁ niṭṭhitaṁ.",
  "pli-tv-pvr3:60.0_0": "2.9. Theyyasatthasamuṭṭhāna",
  "pli-tv-pvr3:60.1_0": "Theyyasatthaṁ upassuti,",
  "pli-tv-pvr3:60.2_0": "sūpaviññāpanena ca;",
  "pli-tv-pvr3:60.3_0": "Rattichannañca okāsaṁ,",
  "pli-tv-pvr3:60.4_0": "ete byūhena sattamā.",
  "pli-tv-pvr3:61.1_0": "Kāyacittena jāyanti,",
  "pli-tv-pvr3:61.2_0": "na te jāyanti vācato;",
  "pli-tv-pvr3:61.3_0": "Tīhi dvārehi jāyanti,",
  "pli-tv-pvr3:61.4_0": "dvisamuṭṭhānikā ime;",
  "pli-tv-pvr3:61.5_0": "Theyyasatthasamuṭṭhānā,",
  "pli-tv-pvr3:61.6_0": "desitādiccabandhunā.",
  "pli-tv-pvr3:62.1_0": "Theyyasatthasamuṭṭhānaṁ niṭṭhitaṁ.",
  "pli-tv-pvr3:63.0_0": "2.10. Dhammadesanāsamuṭṭhāna",
  "pli-tv-pvr3:63.1_0": "Chattapāṇissa saddhammaṁ,",
  "pli-tv-pvr3:63.2_0": "na desenti tathāgatā;",
  "pli-tv-pvr3:63.3_0": "Evameva daṇḍapāṇissa,",
  "pli-tv-pvr3:63.4_0": "satthaāvudhapāṇinaṁ.",
  "pli-tv-pvr3:64.1_0": "Pādukupāhanā yānaṁ,",
  "pli-tv-pvr3:64.2_0": "seyyapallatthikāya ca;",
  "pli-tv-pvr3:64.3_0": "Veṭhitoguṇṭhito ceva,",
  "pli-tv-pvr3:64.4_0": "ekādasamanūnakā.",
  "pli-tv-pvr3:65.1_0": "Vācācittena jāyanti,",
  "pli-tv-pvr3:65.2_0": "na te jāyanti kāyato;",
  "pli-tv-pvr3:65.3_0": "Sabbe ekasamuṭṭhānā,",
  "pli-tv-pvr3:65.4_0": "samakā dhammadesane.",
  "pli-tv-pvr3:66.1_0": "Dhammadesanāsamuṭṭhānaṁ niṭṭhitaṁ.",
  "pli-tv-pvr3:67.0_0": "2.11. Bhūtārocanasamuṭṭhāna",
  "pli-tv-pvr3:67.1_0": "Bhūtaṁ kāyena jāyati,",
  "pli-tv-pvr3:67.2_0": "na vācā na ca cittato;",
  "pli-tv-pvr3:67.3_0": "Vācato ca samuṭṭhāti,",
  "pli-tv-pvr3:67.4_0": "na kāyā na ca cittato.",
  "pli-tv-pvr3:68.1_0": "Kāyavācāya jāyati,",
  "pli-tv-pvr3:68.2_0": "na tu jāyati cittato;",
  "pli-tv-pvr3:68.3_0": "Bhūtārocanakā nāma,",
  "pli-tv-pvr3:68.4_0": "tīhi ṭhānehi jāyati.",
  "pli-tv-pvr3:69.1_0": "Bhūtārocanasamuṭṭhānaṁ niṭṭhitaṁ.",
  "pli-tv-pvr3:70.0_0": "2.12. Corivuṭṭhāpanasamuṭṭhāna",
  "pli-tv-pvr3:70.1_0": "Corī vācāya cittena,",
  "pli-tv-pvr3:70.2_0": "na taṁ jāyati kāyato;",
  "pli-tv-pvr3:70.3_0": "Jāyati tīhi dvārehi,",
  "pli-tv-pvr3:70.4_0": "corivuṭṭhāpanaṁ idaṁ;",
  "pli-tv-pvr3:70.5_0": "Akataṁ dvisamuṭṭhānaṁ,",
  "pli-tv-pvr3:70.6_0": "dhammarājena bhāsitaṁ.",
  "pli-tv-pvr3:71.1_0": "Corivuṭṭhāpanasamuṭṭhānaṁ niṭṭhitaṁ.",
  "pli-tv-pvr3:72.0_0": "2.13. Ananuññātasamuṭṭhāna",
  "pli-tv-pvr3:72.1_0": "Ananuññātaṁ vācāya,",
  "pli-tv-pvr3:72.2_0": "na kāyā na ca cittato;",
  "pli-tv-pvr3:72.3_0": "Jāyati kāyavācāya,",
  "pli-tv-pvr3:72.4_0": "na taṁ jāyati cittato.",
  "pli-tv-pvr3:73.1_0": "Jāyati vācācittena,",
  "pli-tv-pvr3:73.2_0": "na taṁ jāyati kāyato;",
  "pli-tv-pvr3:73.3_0": "Jāyati tīhi dvārehi,",
  "pli-tv-pvr3:73.4_0": "akataṁ catuṭhānikaṁ.",
  "pli-tv-pvr3:74.1_0": "Ananuññātasamuṭṭhānaṁ niṭṭhitaṁ.",
  "pli-tv-pvr3:75.1_0": "Samuṭṭhānañhi saṅkhepaṁ,",
  "pli-tv-pvr3:75.2_0": "dasa tīṇi sudesitaṁ;",
  "pli-tv-pvr3:75.3_0": "Asammohakaraṁ ṭhānaṁ,",
  "pli-tv-pvr3:75.4_0": "nettidhammānulomikaṁ;",
  "pli-tv-pvr3:75.5_0": "Dhārayanto imaṁ viññū,",
  "pli-tv-pvr3:75.6_0": "samuṭṭhāne na muyhatīti.",
  "pli-tv-pvr3:76.1_0": "Samuṭṭhānasīsasaṅkhepo niṭṭhito."
}