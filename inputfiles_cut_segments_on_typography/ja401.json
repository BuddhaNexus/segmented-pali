{
  "ja401:0.1_0": "Jātaka",
  "ja401:0.2_0": "Sattakanipāta",
  "ja401:0.3_0": "Kukkuvagga",
  "ja401:0.4_0": "6. Paṇṇakajātaka",
  "ja401:1.1_0": "“Paṇṇakaṁ tikhiṇadhāraṁ,",
  "ja401:1.2_0": "Asiṁ sampannapāyinaṁ;",
  "ja401:1.3_0": "Parisāyaṁ puriso gilati,",
  "ja401:1.4_0": "Kiṁ dukkarataraṁ tato;",
  "ja401:1.5_0": "Yadaññaṁ dukkaraṁ ṭhānaṁ,",
  "ja401:1.6_0": "Taṁ me akkhāhi pucchito”.",
  "ja401:2.1_0": "“Gileyya puriso lobhā,",
  "ja401:2.2_0": "asiṁ sampannapāyinaṁ;",
  "ja401:2.3_0": "Yo ca vajjā dadāmīti,",
  "ja401:2.4_0": "taṁ dukkarataraṁ tato;",
  "ja401:2.5_0": "Sabbaññaṁ sukaraṁ ṭhānaṁ,",
  "ja401:2.6_0": "evaṁ jānāhi maddava”.",
  "ja401:3.1_0": "“Byākāsi āyuro pañhaṁ,",
  "ja401:3.2_0": "atthaṁ dhammassa kovido;",
  "ja401:3.3_0": "Pukkusaṁ dāni pucchāmi,",
  "ja401:3.4_0": "kiṁ dukkarataraṁ tato;",
  "ja401:3.5_0": "Yadaññaṁ dukkaraṁ ṭhānaṁ,",
  "ja401:3.6_0": "taṁ me akkhāhi pucchito”.",
  "ja401:4.1_0": "“Na vācamupajīvanti,",
  "ja401:4.2_0": "aphalaṁ giramudīritaṁ;",
  "ja401:4.3_0": "Yo ca datvā avākayirā,",
  "ja401:4.4_0": "taṁ dukkarataraṁ tato;",
  "ja401:4.5_0": "Sabbaññaṁ sukaraṁ ṭhānaṁ,",
  "ja401:4.6_0": "evaṁ jānāhi maddava”.",
  "ja401:5.1_0": "“Byākāsi pukkuso pañhaṁ,",
  "ja401:5.2_0": "atthaṁ dhammassa kovido;",
  "ja401:5.3_0": "Senakaṁ dāni pucchāmi,",
  "ja401:5.4_0": "kiṁ dukkarataraṁ tato;",
  "ja401:5.5_0": "Yadaññaṁ dukkaraṁ ṭhānaṁ,",
  "ja401:5.6_0": "taṁ me akkhāhi pucchito”.",
  "ja401:6.1_0": "“Dadeyya puriso dānaṁ,",
  "ja401:6.2_0": "appaṁ vā yadi vā bahuṁ;",
  "ja401:6.3_0": "Yo ca datvā nānutappe,",
  "ja401:6.4_0": "taṁ dukkarataraṁ tato;",
  "ja401:6.5_0": "Sabbaññaṁ sukaraṁ ṭhānaṁ,",
  "ja401:6.6_0": "evaṁ jānāhi maddava”.",
  "ja401:7.1_0": "“Byākāsi āyuro pañhaṁ,",
  "ja401:7.2_0": "atho pukkusaporiso;",
  "ja401:7.3_0": "Sabbe pañhe atibhoti,",
  "ja401:7.4_0": "yathā bhāsati senako”ti.",
  "ja401:8.1_0": "Paṇṇakajātakaṁ chaṭṭhaṁ."
}