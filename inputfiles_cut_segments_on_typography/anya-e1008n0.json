{
  "anya-e1008n0:0_0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1008n0:1_0": "Cāṇakyanītipāḷi",
  "anya-e1008n0:2_0": "1.",
  "anya-e1008n0:3_0": "Nānāsattho ddhataṃ vakkhe,",
  "anya-e1008n0:4_0": "Rāja nīti samuccayaṃ;",
  "anya-e1008n0:5_0": "Sabba bīja midaṃ satthaṃ,",
  "anya-e1008n0:6_0": "Cāṇakyaṃ sārasaṅgahaṃ.",
  "anya-e1008n0:7_0": "2.",
  "anya-e1008n0:8_0": "Mūlasuttaṃ pavakkhāmi,",
  "anya-e1008n0:9_0": "Cāṇakyena yathoditaṃ;",
  "anya-e1008n0:10_0": "Yassa viññāna mattena,",
  "anya-e1008n0:11_0": "Mūḷho bhavati paṇḍito.",
  "anya-e1008n0:12_0": "3.",
  "anya-e1008n0:13_0": "Viduttaṃ narapaccañca,",
  "anya-e1008n0:14_0": "Nevatulyaṃ kudācanaṃ;",
  "anya-e1008n0:15_0": "Sadese pujjate rājā,",
  "anya-e1008n0:16_0": "Vidū sabbattha pujjate.",
  "anya-e1008n0:17_0": "4.",
  "anya-e1008n0:18_0": "Paṇḍite ca guṇā sabbe,",
  "anya-e1008n0:19_0": "Mūḷhe dosā hi kevalaṃ;",
  "anya-e1008n0:20_0": "Tasmā mūḷhasahassesu,",
  "anya-e1008n0:21_0": "Pañño eko visisyate.",
  "anya-e1008n0:22_0": "5.",
  "anya-e1008n0:23_0": "Mātariva paradāresu,",
  "anya-e1008n0:24_0": "Paradabbesu leṭṭuva;",
  "anya-e1008n0:25_0": "Attaniva sabbabhūtesu,",
  "anya-e1008n0:26_0": "Yo passati sapaṇḍito.",
  "anya-e1008n0:27_0": "6.",
  "anya-e1008n0:28_0": "Kiṃkulena visālena,",
  "anya-e1008n0:29_0": "Guṇahīno tu yonaro;",
  "anya-e1008n0:30_0": "Akulinopi satthañño,",
  "anya-e1008n0:31_0": "Devatāhipi pujjate.",
  "anya-e1008n0:32_0": "7.",
  "anya-e1008n0:33_0": "Rūpayobbanasampannā,",
  "anya-e1008n0:34_0": "Visālakulasambhavā;",
  "anya-e1008n0:35_0": "Vijjāhīnā nasobhante,",
  "anya-e1008n0:36_0": "Niggandhā iva kiṃsukā.",
  "anya-e1008n0:37_0": "8.",
  "anya-e1008n0:38_0": "Tārānaṃ bhūsaṇaṃ cando,",
  "anya-e1008n0:39_0": "Nārīnaṃ bhūsaṇaṃ pati;",
  "anya-e1008n0:40_0": "Puthabyā bhūsaṇaṃ rājā,",
  "anya-e1008n0:41_0": "Vijjā sabbassa bhūsaṇaṃ.",
  "anya-e1008n0:42_0": "9.",
  "anya-e1008n0:43_0": "Mātā sattu pitā verī,",
  "anya-e1008n0:44_0": "Yena bālo napāṭṭhito;",
  "anya-e1008n0:45_0": "Na sobhate sabhāmajjhe,",
  "anya-e1008n0:46_0": "Haṃsamajjhe bakoyathā.",
  "anya-e1008n0:47_0": "10.",
  "anya-e1008n0:48_0": "Varameko guṇī putto,",
  "anya-e1008n0:49_0": "Na ca mūḷhasatehipi;",
  "anya-e1008n0:50_0": "Eko cando tamo hanti,",
  "anya-e1008n0:51_0": "Na ca tāragaṇehipi.",
  "anya-e1008n0:52_0": "11.",
  "anya-e1008n0:53_0": "Lālaye pañcavassāni,",
  "anya-e1008n0:54_0": "Dasavassāni tālaye;",
  "anya-e1008n0:55_0": "Patte tu soḷase vasse,",
  "anya-e1008n0:56_0": "Puttaṃ mittaṃva ācare.",
  "anya-e1008n0:57_0": "12.",
  "anya-e1008n0:58_0": "Lālane bahavo dosā,",
  "anya-e1008n0:59_0": "Tālane bahavo guṇā;",
  "anya-e1008n0:60_0": "Tasmā puttañca sissañca,",
  "anya-e1008n0:61_0": "Tālaye na tu lālaye.",
  "anya-e1008n0:62_0": "13.",
  "anya-e1008n0:63_0": "Ekenāpi suvakkhena,",
  "anya-e1008n0:64_0": "Pupphitena sugandhinā;",
  "anya-e1008n0:65_0": "Vāsitassa vanaṃ sabbaṃ,",
  "anya-e1008n0:66_0": "Suputtena kulaṃyathā.",
  "anya-e1008n0:67_0": "14.",
  "anya-e1008n0:68_0": "Ekassāpi kuvakkhassa,",
  "anya-e1008n0:69_0": "Koṭaraṭṭhena agginā;",
  "anya-e1008n0:70_0": "Dayhate hi vanaṃ sabbaṃ,",
  "anya-e1008n0:71_0": "Kuputtena kulaṃ yathā.",
  "anya-e1008n0:72_0": "15.",
  "anya-e1008n0:73_0": "Dūrato sobhate mūḷho,",
  "anya-e1008n0:74_0": "Lambamāna paṭāvuto;",
  "anya-e1008n0:75_0": "Tāvañca sobhate mūḷho,",
  "anya-e1008n0:76_0": "Yāva kiñci nabhāsate.",
  "anya-e1008n0:77_0": "16.",
  "anya-e1008n0:78_0": "Visato amataṃ gāyhaṃ,",
  "anya-e1008n0:79_0": "Amejjhāapi kañcanaṃ;",
  "anya-e1008n0:80_0": "Nīcato uttamā vijjā,",
  "anya-e1008n0:81_0": "Thīratnaṃ dukkulāapi.",
  "anya-e1008n0:82_0": "17.",
  "anya-e1008n0:83_0": "Ussave byasaneceva,",
  "anya-e1008n0:84_0": "Dubbhikkhe sattuviggahe;",
  "anya-e1008n0:85_0": "Rājadvāre susāneca,",
  "anya-e1008n0:86_0": "Yo tiṭṭhati sabandhavo.",
  "anya-e1008n0:87_0": "18.",
  "anya-e1008n0:88_0": "Parokkhe kiccahantāraṃ,",
  "anya-e1008n0:89_0": "Paccakkhe piyavādinaṃ;",
  "anya-e1008n0:90_0": "Vajjaye tādisaṃ mittaṃ,",
  "anya-e1008n0:91_0": "Visakumbhaṃ payomukhaṃ.",
  "anya-e1008n0:92_0": "19.",
  "anya-e1008n0:93_0": "Sakiṃ duṭṭhañca mittaṃ yo,",
  "anya-e1008n0:94_0": "Puna sandhātu micchati;",
  "anya-e1008n0:95_0": "Samaccu mupagaṇhāti,",
  "anya-e1008n0:96_0": "Gabbha massatarī yathā.",
  "anya-e1008n0:97_0": "20.",
  "anya-e1008n0:98_0": "Na vissase avissatthaṃ,",
  "anya-e1008n0:99_0": "Mittañcāpi na vissase;",
  "anya-e1008n0:100_0": "Kadāci kupitaṃ mittaṃ,",
  "anya-e1008n0:101_0": "Sabbadosaṃ pakāsaye.",
  "anya-e1008n0:102_0": "21.",
  "anya-e1008n0:103_0": "Jāniyā pesane bhacce,",
  "anya-e1008n0:104_0": "Bandhave byasanāgame;",
  "anya-e1008n0:105_0": "Mittañcā padikāleca,",
  "anya-e1008n0:106_0": "Bhariyañca vibhavakkhaye.",
  "anya-e1008n0:107_0": "22.",
  "anya-e1008n0:108_0": "Upakāraggahitena,",
  "anya-e1008n0:109_0": "Sattunāsattumuddhare;",
  "anya-e1008n0:110_0": "Pādalaggaṃ karaṭṭhena,",
  "anya-e1008n0:111_0": "Kaṇṭakeneva kaṇṭakaṃ.",
  "anya-e1008n0:112_0": "23.",
  "anya-e1008n0:113_0": "Na mittaṃ koci kassaci,",
  "anya-e1008n0:114_0": "Na koci ripu kassaci;",
  "anya-e1008n0:115_0": "Kāraṇena hi ñāyati,",
  "anya-e1008n0:116_0": "Mittāni ca ripū tathā.",
  "anya-e1008n0:117_0": "24.",
  "anya-e1008n0:118_0": "Dujjano piyavādī ca,",
  "anya-e1008n0:119_0": "Netaṃ vissāsakāraṇaṃ;",
  "anya-e1008n0:120_0": "Madhu tiṭṭhati jivhagge,",
  "anya-e1008n0:121_0": "Hadaye tu halāhalaṃ.",
  "anya-e1008n0:122_0": "25.",
  "anya-e1008n0:123_0": "Dujjano parihantabbo,",
  "anya-e1008n0:124_0": "Vijjāyā laṅkatopi saṃ;",
  "anya-e1008n0:125_0": "Maṇinā bhūsito sappo,",
  "anya-e1008n0:126_0": "Kime so nabhayaṃ karo.",
  "anya-e1008n0:127_0": "26.",
  "anya-e1008n0:128_0": "Sappo kūro khalo kūro,",
  "anya-e1008n0:129_0": "Sappā kūrataro khalo;",
  "anya-e1008n0:130_0": "Manto sadhivaso sappo,",
  "anya-e1008n0:131_0": "Khalo kena nivāyyate.",
  "anya-e1008n0:132_0": "27.",
  "anya-e1008n0:133_0": "Nakhīnañca nadīnañca,",
  "anya-e1008n0:134_0": "Siṅgīnaṃ satthapāṇinaṃ;",
  "anya-e1008n0:135_0": "Vissāso nevakātabbo,",
  "anya-e1008n0:136_0": "Thīsu rājakulesu ca.",
  "anya-e1008n0:137_0": "28.",
  "anya-e1008n0:138_0": "Hatthī hatthasahassena,",
  "anya-e1008n0:139_0": "Satahatthena vājino;",
  "anya-e1008n0:140_0": "Siṅgino dasahatthena,",
  "anya-e1008n0:141_0": "Ṭhānacāgena dujjano.",
  "anya-e1008n0:142_0": "29.",
  "anya-e1008n0:143_0": "Āpadatthaṃ dhanaṃ rakkhe,",
  "anya-e1008n0:144_0": "Dāraṃ rakkhe dhanehipi;",
  "anya-e1008n0:145_0": "Attānaṃ satataṃ rakkhe,",
  "anya-e1008n0:146_0": "Dārehipi dhanehipi.",
  "anya-e1008n0:147_0": "30.",
  "anya-e1008n0:148_0": "Paradāraṃ paradabbaṃ,",
  "anya-e1008n0:149_0": "Parivādaṃ parassa ca;",
  "anya-e1008n0:150_0": "Parihāsaṃ guruṭṭhāne,",
  "anya-e1008n0:151_0": "Cāpalyañca vivajjaye.",
  "anya-e1008n0:152_0": "31.",
  "anya-e1008n0:153_0": "Caje ekaṃ kulassatthe,",
  "anya-e1008n0:154_0": "Gāmassatthe kulaṃ caje;",
  "anya-e1008n0:155_0": "Gāma janapadassatthe,",
  "anya-e1008n0:156_0": "Attatthe pathaviṃ caje.",
  "anya-e1008n0:157_0": "32.",
  "anya-e1008n0:158_0": "Calatyekena pādena,",
  "anya-e1008n0:159_0": "Tiṭṭha tyekena buddhimā;",
  "anya-e1008n0:160_0": "Nāsamikkhya paraṃ ṭhānaṃ,",
  "anya-e1008n0:161_0": "Pubbamāyatanaṃ caje.",
  "anya-e1008n0:162_0": "33.",
  "anya-e1008n0:163_0": "Luddha matthena gaṇheyya,",
  "anya-e1008n0:164_0": "Thaddha mañjalī kammunā;",
  "anya-e1008n0:165_0": "Mūḷhaṃ chando nuvattena,",
  "anya-e1008n0:166_0": "Tathā tathena paṇḍitaṃ.",
  "anya-e1008n0:167_0": "34.",
  "anya-e1008n0:168_0": "Atthanāsaṃ manotāpaṃ,",
  "anya-e1008n0:169_0": "Gehe duccaritāni ca;",
  "anya-e1008n0:170_0": "Vañcanañca pamāṇañca,",
  "anya-e1008n0:171_0": "Matimā na pakāsaye.",
  "anya-e1008n0:172_0": "35.",
  "anya-e1008n0:173_0": "Dhanadhaññappayogesu,",
  "anya-e1008n0:174_0": "Tathā vijjāgamesu ca;",
  "anya-e1008n0:175_0": "Āhāre byavahāre ca,",
  "anya-e1008n0:176_0": "Cattalajjo sadā bhave.",
  "anya-e1008n0:177_0": "36.",
  "anya-e1008n0:178_0": "Dhanino sotthiyo rājā,",
  "anya-e1008n0:179_0": "Nadī vejjo tu pañcamo;",
  "anya-e1008n0:180_0": "Pañca yatra navijjante,",
  "anya-e1008n0:181_0": "Tatra vāsaṃ nakāraye.",
  "anya-e1008n0:182_0": "37.",
  "anya-e1008n0:183_0": "Yasmiṃdese na sammānaṃ,",
  "anya-e1008n0:184_0": "Na pīti naca bandhavā;",
  "anya-e1008n0:185_0": "Na ca vijjāgamo koci,",
  "anya-e1008n0:186_0": "Taṃdesaṃ parivajjaye.",
  "anya-e1008n0:187_0": "38.",
  "anya-e1008n0:188_0": "Manasā cintitaṃ kammaṃ,",
  "anya-e1008n0:189_0": "Vacasā napakāsaye;",
  "anya-e1008n0:190_0": "Aññalakkhita kāriyassa,",
  "anya-e1008n0:191_0": "Yato siddhi najāyate.",
  "anya-e1008n0:192_0": "39.",
  "anya-e1008n0:193_0": "Kudesañca kuvuttiñca,",
  "anya-e1008n0:194_0": "Kubhariyaṃ kunadiṃ tathā;",
  "anya-e1008n0:195_0": "Kudabbañca kubhojjañca,",
  "anya-e1008n0:196_0": "Vajjaye tu vicakkhaṇo.",
  "anya-e1008n0:197_0": "40.",
  "anya-e1008n0:198_0": "Iṇasesoggi seso ca,",
  "anya-e1008n0:199_0": "Byādhiseso tatheva ca;",
  "anya-e1008n0:200_0": "Puna ca vaḍḍhate yasmā,",
  "anya-e1008n0:201_0": "Tasmā sesaṃ nakāraye.",
  "anya-e1008n0:202_0": "41.",
  "anya-e1008n0:203_0": "Cintā jaro manussānaṃ,",
  "anya-e1008n0:204_0": "Vatthānaṃ ātapo jaro;",
  "anya-e1008n0:205_0": "Asobhagyaṃ jaro thīnaṃ,",
  "anya-e1008n0:206_0": "Assānaṃ methunaṃ jaro.",
  "anya-e1008n0:207_0": "42.",
  "anya-e1008n0:208_0": "Atthi putto vase yassa,",
  "anya-e1008n0:209_0": "Bhacco bhariyā tatheva ca;",
  "anya-e1008n0:210_0": "Abhāve pyatisantoso,",
  "anya-e1008n0:211_0": "Saggaṭṭho so mahītale.",
  "anya-e1008n0:212_0": "43.",
  "anya-e1008n0:213_0": "Duṭṭhā bhariyā saṭhaṃ mittaṃ,",
  "anya-e1008n0:214_0": "Bhacco cuttaradāyako;",
  "anya-e1008n0:215_0": "Sa sappeca gahe vāso,",
  "anya-e1008n0:216_0": "Maccureva nasaṃsayo.",
  "anya-e1008n0:217_0": "44.",
  "anya-e1008n0:218_0": "Mātā yassa gehe natthi,",
  "anya-e1008n0:219_0": "Bhariyācā piyavādinī;",
  "anya-e1008n0:220_0": "Araññaṃ tena gantabbaṃ,",
  "anya-e1008n0:221_0": "Yathā raññaṃ tathāgahaṃ.",
  "anya-e1008n0:222_0": "45.",
  "anya-e1008n0:223_0": "Iṇakattā pitā sattu,",
  "anya-e1008n0:224_0": "Mātā ca byabhicārinī;",
  "anya-e1008n0:225_0": "Bhariyā rūpavatī sattu,",
  "anya-e1008n0:226_0": "Putto sattu apaṇḍito.",
  "anya-e1008n0:227_0": "46.",
  "anya-e1008n0:228_0": "Kokilānaṃ saro rūpaṃ,",
  "anya-e1008n0:229_0": "Nārī rūpaṃ patibbatā;",
  "anya-e1008n0:230_0": "Vijjā rūpaṃ kurūpānaṃ,",
  "anya-e1008n0:231_0": "Khamā rūpaṃ tapassinaṃ.",
  "anya-e1008n0:232_0": "47.",
  "anya-e1008n0:233_0": "Avijjaṃ jīvanaṃ suññaṃ,",
  "anya-e1008n0:234_0": "Disā suññā abandhavā;",
  "anya-e1008n0:235_0": "Puttahīnaṃ gahaṃ suññaṃ,",
  "anya-e1008n0:236_0": "Sabbasuññā daliddatā.",
  "anya-e1008n0:237_0": "48.",
  "anya-e1008n0:238_0": "Adātā vaṃsadosena,",
  "anya-e1008n0:239_0": "Kammadosā daliddatā;",
  "anya-e1008n0:240_0": "Ummādā mātudosena,",
  "anya-e1008n0:241_0": "Pitudosena mūḷhatā.",
  "anya-e1008n0:242_0": "49.",
  "anya-e1008n0:243_0": "Guru aggi dvijādīnaṃ,",
  "anya-e1008n0:244_0": "Vaṇṇānaṃ brāhmaṇo guru;",
  "anya-e1008n0:245_0": "Pati reko gurutthīnaṃ,",
  "anya-e1008n0:246_0": "Sabbassābhyāgato guru.",
  "anya-e1008n0:247_0": "50.",
  "anya-e1008n0:248_0": "Atidabbe hatā laṅkā,",
  "anya-e1008n0:249_0": "Atimāne ca koravā;",
  "anya-e1008n0:250_0": "Atidāne balībaddho,",
  "anya-e1008n0:251_0": "Sabbamaccanta gahitaṃ.",
  "anya-e1008n0:252_0": "51.",
  "anya-e1008n0:253_0": "Vatthahīno tvalaṅkāro,",
  "anya-e1008n0:254_0": "Ghatahīnañca bhojanaṃ;",
  "anya-e1008n0:255_0": "Thanahīnā ca yānārī,",
  "anya-e1008n0:256_0": "Vijjāhīnañca jīvanaṃ.",
  "anya-e1008n0:257_0": "52.",
  "anya-e1008n0:258_0": "Bhojjaṃ bhojanasatti ca,",
  "anya-e1008n0:259_0": "Ratisatti varā thiyo;",
  "anya-e1008n0:260_0": "Vibhavo dānasatti ca,",
  "anya-e1008n0:261_0": "Nāppassa tapaso phalaṃ.",
  "anya-e1008n0:262_0": "53.",
  "anya-e1008n0:263_0": "Puttappayojanā dārā,",
  "anya-e1008n0:264_0": "Putto piṇḍappayojano;",
  "anya-e1008n0:265_0": "Hitappayojanaṃ mittaṃ,",
  "anya-e1008n0:266_0": "Dhanaṃ sabbappayojanaṃ.",
  "anya-e1008n0:267_0": "54.",
  "anya-e1008n0:268_0": "Dullabhaṃ pākatikaṃ vākyaṃ,",
  "anya-e1008n0:269_0": "Dullabho khemakaro suto;",
  "anya-e1008n0:270_0": "Dullabhā sadisī jāyā,",
  "anya-e1008n0:271_0": "Dullabho sajano piyo.",
  "anya-e1008n0:272_0": "55.",
  "anya-e1008n0:273_0": "Selesele namāṇikkaṃ,",
  "anya-e1008n0:274_0": "Muttikaṃ na gajegaje;",
  "anya-e1008n0:275_0": "Sādhavo na hi sabbatra,",
  "anya-e1008n0:276_0": "Candanaṃ na vanevane.",
  "anya-e1008n0:277_0": "56.",
  "anya-e1008n0:278_0": "Asoco niddhano pañño,",
  "anya-e1008n0:279_0": "Asoco paṇḍitabandhavo;",
  "anya-e1008n0:280_0": "Asocā vidhavā nārī,",
  "anya-e1008n0:281_0": "Puttanatta patiṭṭhitā.",
  "anya-e1008n0:282_0": "57.",
  "anya-e1008n0:283_0": "Avijjo puriso soco,",
  "anya-e1008n0:284_0": "Socaṃ methuna mappajaṃ;",
  "anya-e1008n0:285_0": "Nirāhārā pajā socā,",
  "anya-e1008n0:286_0": "Socaṃ rajja marājakaṃ.",
  "anya-e1008n0:287_0": "58.",
  "anya-e1008n0:288_0": "Kulehi saha sampakkaṃ,",
  "anya-e1008n0:289_0": "Paṇḍitehi ca mittataṃ;",
  "anya-e1008n0:290_0": "Ñātībhi ca samaṃ melaṃ,",
  "anya-e1008n0:291_0": "Kubbāno navinassati.",
  "anya-e1008n0:292_0": "59.",
  "anya-e1008n0:293_0": "Kaṭṭhā vutti parādhinā,",
  "anya-e1008n0:294_0": "Kaṭṭho vāso nirāsayo;",
  "anya-e1008n0:295_0": "Niddhano byavasāyo ca,",
  "anya-e1008n0:296_0": "Sabbakaṭṭhā daliddatā.",
  "anya-e1008n0:297_0": "60.",
  "anya-e1008n0:298_0": "Takkarassa kuto dhammo,",
  "anya-e1008n0:299_0": "Dujjanassa kuto khamā;",
  "anya-e1008n0:300_0": "Vesiyā ca kuto sneho,",
  "anya-e1008n0:301_0": "Kuto saccañca kāminaṃ.",
  "anya-e1008n0:302_0": "61.",
  "anya-e1008n0:303_0": "Pesitassa kuto mānaṃ,",
  "anya-e1008n0:304_0": "Kopanassa kuto sukhaṃ;",
  "anya-e1008n0:305_0": "Thīnaṃ kuto satittañca,",
  "anya-e1008n0:306_0": "Kuto mettī khalassa ca.",
  "anya-e1008n0:307_0": "62.",
  "anya-e1008n0:308_0": "Dubbalassa balaṃ rājā,",
  "anya-e1008n0:309_0": "Bālānaṃ rodanaṃ balaṃ;",
  "anya-e1008n0:310_0": "Balaṃmūḷhassa monittaṃ,",
  "anya-e1008n0:311_0": "Corānaṃ atathaṃ balaṃ.",
  "anya-e1008n0:312_0": "63.",
  "anya-e1008n0:313_0": "Yo dhuvāni pariccajja,",
  "anya-e1008n0:314_0": "Adhuvaṃ parisevati;",
  "anya-e1008n0:315_0": "Dhuvāni tassa nassanti,",
  "anya-e1008n0:316_0": "Adhuvaṃ naṭṭhameva ca.",
  "anya-e1008n0:317_0": "64.",
  "anya-e1008n0:318_0": "Sukkaṃ maṃsaṃ thiyo vuddhā,",
  "anya-e1008n0:319_0": "Bālakka taruṇaṃ dadhi;",
  "anya-e1008n0:320_0": "Pabhāte methunaṃ niddā,",
  "anya-e1008n0:321_0": "Sajju pāṇaharāni cha;",
  "anya-e1008n0:322_0": "65.",
  "anya-e1008n0:323_0": "Sajju maṃsaṃ navannañca,",
  "anya-e1008n0:324_0": "Bālā thī khīrabhojanaṃ;",
  "anya-e1008n0:325_0": "Ghatamuṇhodakañceva,",
  "anya-e1008n0:326_0": "Sajju pāṇakarāni cha.",
  "anya-e1008n0:327_0": "66.",
  "anya-e1008n0:328_0": "Sīhādekaṃ bakādekaṃ,",
  "anya-e1008n0:329_0": "Cha sunā tīṇi gadrabhā;",
  "anya-e1008n0:330_0": "Vāyasā catu sikkhetha,",
  "anya-e1008n0:331_0": "Cattāri kukkuṭādapi.",
  "anya-e1008n0:332_0": "67.",
  "anya-e1008n0:333_0": "Pabhūtamappakiccaṃ vā,",
  "anya-e1008n0:334_0": "Yonaro kattumicchati;",
  "anya-e1008n0:335_0": "Saṃyatanena kattabbaṃ,",
  "anya-e1008n0:336_0": "Sīhādekaṃ pakittitaṃ.",
  "anya-e1008n0:337_0": "68.",
  "anya-e1008n0:338_0": "Sabbindriyāni saṃyama,",
  "anya-e1008n0:339_0": "Bakova patito jano;",
  "anya-e1008n0:340_0": "Kāladesopapannāni,",
  "anya-e1008n0:341_0": "Sabbakiccāni sādhaye.",
  "anya-e1008n0:342_0": "69.",
  "anya-e1008n0:343_0": "Bahvāsī sāppasantuṭṭho,",
  "anya-e1008n0:344_0": "Suniddo sīghacetano;",
  "anya-e1008n0:345_0": "Pabhubhatto ca sūro ca,",
  "anya-e1008n0:346_0": "Ñātabbā cha sunā guṇā.",
  "anya-e1008n0:347_0": "70.",
  "anya-e1008n0:348_0": "Avissāmaṃ vahe bhāraṃ,",
  "anya-e1008n0:349_0": "Sītuṇhañca navindati;",
  "anya-e1008n0:350_0": "Sa santoso tathā niccaṃ,",
  "anya-e1008n0:351_0": "Tīṇi sikkhetha gadrabhā.",
  "anya-e1008n0:352_0": "71.",
  "anya-e1008n0:353_0": "Guḷhamethunadhammañca,",
  "anya-e1008n0:354_0": "Kālekāle ca saṅgahaṃ;",
  "anya-e1008n0:355_0": "Appamādamanālasyaṃ,",
  "anya-e1008n0:356_0": "Catu sikkhetha vāyasā.",
  "anya-e1008n0:357_0": "72.",
  "anya-e1008n0:358_0": "Yuddhañca pātaruṭṭhānaṃ,",
  "anya-e1008n0:359_0": "Bhojanaṃ saha bandhuhi;",
  "anya-e1008n0:360_0": "Thiyaṃ āpadaggataṃ rakkhe,",
  "anya-e1008n0:361_0": "Catu sikkhetha kukkuṭā.",
  "anya-e1008n0:362_0": "73.",
  "anya-e1008n0:363_0": "Kotibhāro samatthānaṃ,",
  "anya-e1008n0:364_0": "Kiṃdūraṃ byavasāyinaṃ;",
  "anya-e1008n0:365_0": "Ko videso savijjānaṃ,",
  "anya-e1008n0:366_0": "Ko paro piyavādinaṃ.",
  "anya-e1008n0:367_0": "74.",
  "anya-e1008n0:368_0": "Bhayassa kathito pantho,",
  "anya-e1008n0:369_0": "Indriyānamasaṃyamo;",
  "anya-e1008n0:370_0": "Tajjayo sampadāmaggo,",
  "anya-e1008n0:371_0": "Yeniṭṭhaṃ tena gamyate.",
  "anya-e1008n0:372_0": "75.",
  "anya-e1008n0:373_0": "Na ca vijjāsamo bandhu,",
  "anya-e1008n0:374_0": "Na ca byādhisamo ripu;",
  "anya-e1008n0:375_0": "Nacāpaccasamo sneho,",
  "anya-e1008n0:376_0": "Na ca devā paraṃ balaṃ.",
  "anya-e1008n0:377_0": "76.",
  "anya-e1008n0:378_0": "Samuddāvaraṇā bhūmi,",
  "anya-e1008n0:379_0": "Pākārāvaraṇaṃ gahaṃ;",
  "anya-e1008n0:380_0": "Narindāvaraṇā desā,",
  "anya-e1008n0:381_0": "Cārittāvaraṇā thiyo.",
  "anya-e1008n0:382_0": "77.",
  "anya-e1008n0:383_0": "Ghatakumbhasamā nārī,",
  "anya-e1008n0:384_0": "Tattaṅgāra samo pumā;",
  "anya-e1008n0:385_0": "Tasmā ghatañca aggiñca,",
  "anya-e1008n0:386_0": "Nekatra thapaye budho.",
  "anya-e1008n0:387_0": "78.",
  "anya-e1008n0:388_0": "Āhāro dviguṇo thīnaṃ,",
  "anya-e1008n0:389_0": "Buddhi tāsaṃ catugguṇo;",
  "anya-e1008n0:390_0": "Chaguṇo byavasāyo ca,",
  "anya-e1008n0:391_0": "Kāmocaṭṭhaguṇo mato.",
  "anya-e1008n0:392_0": "79.",
  "anya-e1008n0:393_0": "Jiṇṇamannaṃ pasaṃseyya,",
  "anya-e1008n0:394_0": "Bhariyaṃ gatayobbanaṃ;",
  "anya-e1008n0:395_0": "Raṇā paccāgataṃ sūraṃ,",
  "anya-e1008n0:396_0": "Sassañca gehamāgataṃ.",
  "anya-e1008n0:397_0": "80.",
  "anya-e1008n0:398_0": "Asantuṭṭhā dvijā naṭṭhā,",
  "anya-e1008n0:399_0": "Santuṭṭhāiva pāthivā;",
  "anya-e1008n0:400_0": "Salajjā gaṇikā naṭṭhā,",
  "anya-e1008n0:401_0": "Nillajjā ca kulitthiyo.",
  "anya-e1008n0:402_0": "81.",
  "anya-e1008n0:403_0": "Avaṃsapatito rājā,",
  "anya-e1008n0:404_0": "Mūḷhaputto ca paṇḍito;",
  "anya-e1008n0:405_0": "Adhanena dhanaṃ pāpya,",
  "anya-e1008n0:406_0": "Tiṇaṃva maññate janaṃ.",
  "anya-e1008n0:407_0": "82.",
  "anya-e1008n0:408_0": "Brahmahāpi naro pujjo,",
  "anya-e1008n0:409_0": "Yassatthi vipulaṃ dhanaṃ;",
  "anya-e1008n0:410_0": "Sasino tulyavaṃsopi,",
  "anya-e1008n0:411_0": "Niddhano paribhūyate.",
  "anya-e1008n0:412_0": "83.",
  "anya-e1008n0:413_0": "Potthakaṭṭhā tu yāvijjā,",
  "anya-e1008n0:414_0": "Parahatthagataṃ dhanaṃ;",
  "anya-e1008n0:415_0": "Kiccakāle samuppanne,",
  "anya-e1008n0:416_0": "Na sāvijjā na taddhanaṃ.",
  "anya-e1008n0:417_0": "84.",
  "anya-e1008n0:418_0": "Pādapānaṃ bhayaṃ vātā,",
  "anya-e1008n0:419_0": "Padmānaṃ sisirā bhayaṃ;",
  "anya-e1008n0:420_0": "Pabbatānaṃ vajīramhā,",
  "anya-e1008n0:421_0": "Sādhūnaṃ dujjanā bhayaṃ.",
  "anya-e1008n0:422_0": "85.",
  "anya-e1008n0:423_0": "Paññe niyujjamāne tu,",
  "anya-e1008n0:424_0": "Santi rañño tayoguṇā;",
  "anya-e1008n0:425_0": "Yaso sagganivāso ca,",
  "anya-e1008n0:426_0": "Vipulo ca dhanāgamo.",
  "anya-e1008n0:427_0": "86.",
  "anya-e1008n0:428_0": "Mūḷhe niyujjamānetu,",
  "anya-e1008n0:429_0": "Khattiyassāguṇā tayo;",
  "anya-e1008n0:430_0": "Ayaso catthanāso ca,",
  "anya-e1008n0:431_0": "Narake gamanaṃ tathā.",
  "anya-e1008n0:432_0": "87.",
  "anya-e1008n0:433_0": "Bahūmūḷhasaṅghātehi,",
  "anya-e1008n0:434_0": "Aññoññapasuvuttibhi;",
  "anya-e1008n0:435_0": "Pacchādyante guṇā sabbe,",
  "anya-e1008n0:436_0": "Meghehiva divākaro.",
  "anya-e1008n0:437_0": "88.",
  "anya-e1008n0:438_0": "Yassa khettaṃ nadītīre,",
  "anya-e1008n0:439_0": "Bhariyāpi parappiyā;",
  "anya-e1008n0:440_0": "Puttassa vinayo natthi,",
  "anya-e1008n0:441_0": "Maccureva nasaṃsayo.",
  "anya-e1008n0:442_0": "89.",
  "anya-e1008n0:443_0": "Asambhābyaṃ navattabbaṃ,",
  "anya-e1008n0:444_0": "Paccakkhamapi dissate;",
  "anya-e1008n0:445_0": "Silā tarati pānīyaṃ,",
  "anya-e1008n0:446_0": "Gītaṃ gāyati vānaro.",
  "anya-e1008n0:447_0": "90.",
  "anya-e1008n0:448_0": "Subhikkhaṃ kasake niccaṃ,",
  "anya-e1008n0:449_0": "Niccaṃ sukha marogike;",
  "anya-e1008n0:450_0": "Bhariyā bhattu piyā yassa,",
  "anya-e1008n0:451_0": "Tassa niccossavaṃ gahaṃ.",
  "anya-e1008n0:452_0": "91.",
  "anya-e1008n0:453_0": "Helassa kammanāsāya,",
  "anya-e1008n0:454_0": "Buddhināsāya niddhanaṃ;",
  "anya-e1008n0:455_0": "Yācanā mānanāsāya,",
  "anya-e1008n0:456_0": "Kulanāsāya bhojanaṃ.",
  "anya-e1008n0:457_0": "92.",
  "anya-e1008n0:458_0": "Sevitabbo mahāvakkho,",
  "anya-e1008n0:459_0": "Phalacchāyā samanvito;",
  "anya-e1008n0:460_0": "Yadi devā phalaṃ natthi,",
  "anya-e1008n0:461_0": "Chāyā kena nivāraye.",
  "anya-e1008n0:462_0": "93.",
  "anya-e1008n0:463_0": "Paṭhame najjitā vijjā,",
  "anya-e1008n0:464_0": "Dutīye najjitaṃ dhanaṃ;",
  "anya-e1008n0:465_0": "Tatīye najjitaṃ puññaṃ,",
  "anya-e1008n0:466_0": "Catutthe kiṃkarissati.",
  "anya-e1008n0:467_0": "94.",
  "anya-e1008n0:468_0": "Nadīkūleca ye vakkhā,",
  "anya-e1008n0:469_0": "Parahatthagataṃ dhanaṃ;",
  "anya-e1008n0:470_0": "Kiccaṃ thīgocaraṃ yassa,",
  "anya-e1008n0:471_0": "Sabbaṃ taṃ viphalaṃ bhave.",
  "anya-e1008n0:472_0": "95.",
  "anya-e1008n0:473_0": "Kudesamāsajja kutotthasañcayo,",
  "anya-e1008n0:474_0": "Kuputtamāsajja kuto jalañjalī;",
  "anya-e1008n0:475_0": "Kugehiniṃ pāpya gahe kuto sukhaṃ,",
  "anya-e1008n0:476_0": "Kusissamajjhāpayato kuto yaso.",
  "anya-e1008n0:477_0": "96.",
  "anya-e1008n0:478_0": "Kūpodakaṃ vaṭacchāyā,",
  "anya-e1008n0:479_0": "Sāmā thīciṭṭhakālayaṃ;",
  "anya-e1008n0:480_0": "Sītakāle bhave uṇhaṃ,",
  "anya-e1008n0:481_0": "Gimhakāle ca sītalaṃ.",
  "anya-e1008n0:482_0": "97.",
  "anya-e1008n0:483_0": "Visaṃ caṅkamanaṃ rattiṃ,",
  "anya-e1008n0:484_0": "Visaṃ raññonukulatā;",
  "anya-e1008n0:485_0": "Visaṃ thīpi aññāsattā,",
  "anya-e1008n0:486_0": "Visaṃ byādhi avekkhito.",
  "anya-e1008n0:487_0": "98.",
  "anya-e1008n0:488_0": "Duradhītā visaṃ vijjā,",
  "anya-e1008n0:489_0": "Ajiṇṇe bhojanaṃ visaṃ;",
  "anya-e1008n0:490_0": "Visaṃ goṭṭhī daliddassa,",
  "anya-e1008n0:491_0": "Vuddhassa taruṇī visaṃ.",
  "anya-e1008n0:492_0": "99.",
  "anya-e1008n0:493_0": "Padose nihato pantho,",
  "anya-e1008n0:494_0": "Patitā nihatā thiyo;",
  "anya-e1008n0:495_0": "Appabījaṃ hataṃ khettaṃ,",
  "anya-e1008n0:496_0": "Bhaccadosā hato pabhū.",
  "anya-e1008n0:497_0": "100.",
  "anya-e1008n0:498_0": "Hatamasottiyaṃ saddhaṃ,",
  "anya-e1008n0:499_0": "Hato yañño tvadakkhiṇo;",
  "anya-e1008n0:500_0": "Hatā rūpavatī vañjhā,",
  "anya-e1008n0:501_0": "Hataṃ senamanāyakaṃ.",
  "anya-e1008n0:502_0": "101.",
  "anya-e1008n0:503_0": "Vedavedaṅga tattañño,",
  "anya-e1008n0:504_0": "Japahomaparāyano;",
  "anya-e1008n0:505_0": "Āsīvādavacoyutto,",
  "anya-e1008n0:506_0": "Esa rājapurohito.",
  "anya-e1008n0:507_0": "102.",
  "anya-e1008n0:508_0": "Kulasīlaguṇopeto,",
  "anya-e1008n0:509_0": "Sabbadhammaparāyano;",
  "anya-e1008n0:510_0": "Pavīṇo pesanādyakkho,",
  "anya-e1008n0:511_0": "Dhammādyakkho vidhīyate.",
  "anya-e1008n0:512_0": "103.",
  "anya-e1008n0:513_0": "Ayubbedakatābhyāso,",
  "anya-e1008n0:514_0": "Sabbesaṃ piyadassano;",
  "anya-e1008n0:515_0": "Ariyasīlaguṇopeto,",
  "anya-e1008n0:516_0": "Esa vajjo vidhīyate.",
  "anya-e1008n0:517_0": "104.",
  "anya-e1008n0:518_0": "Sakiṃdutta gahitattho,",
  "anya-e1008n0:519_0": "Lahuhattho jitakkharo;",
  "anya-e1008n0:520_0": "Sabbasattha samālokī,",
  "anya-e1008n0:521_0": "Pakaṭṭho nāma lekhako.",
  "anya-e1008n0:522_0": "105.",
  "anya-e1008n0:523_0": "Samattanītisattañño,",
  "anya-e1008n0:524_0": "Vāhane pūjitassamo;",
  "anya-e1008n0:525_0": "Sūravīraguṇopeto,",
  "anya-e1008n0:526_0": "Senādhyakkho vidhīyate.",
  "anya-e1008n0:527_0": "106.",
  "anya-e1008n0:528_0": "Sucī vākyapaṭuppañño,",
  "anya-e1008n0:529_0": "Paracittopalakkhako;",
  "anya-e1008n0:530_0": "Dhīro yathāttha vādī ca,",
  "anya-e1008n0:531_0": "Esa dūto vidhīyate.",
  "anya-e1008n0:532_0": "107.",
  "anya-e1008n0:533_0": "Puttanatta guṇopeto,",
  "anya-e1008n0:534_0": "Satthañño piṭṭhapācako;",
  "anya-e1008n0:535_0": "Sūro ca kathinoceva,",
  "anya-e1008n0:536_0": "Sūpakāro sa uccate.",
  "anya-e1008n0:537_0": "108.",
  "anya-e1008n0:538_0": "Iṅgitā kāratattañño,",
  "anya-e1008n0:539_0": "Balavā piyadassano;",
  "anya-e1008n0:540_0": "Appamādī sadā dakkho,",
  "anya-e1008n0:541_0": "Patihāro sa uccate.",
  "anya-e1008n0:542_0": "109.",
  "anya-e1008n0:543_0": "Yassa natthi sayaṃ paññā,",
  "anya-e1008n0:544_0": "Satthaṃ tassa karoti kiṃ;",
  "anya-e1008n0:545_0": "Locanehi vihīnassa,",
  "anya-e1008n0:546_0": "Dappaṇo kiṃkarissati.",
  "anya-e1008n0:547_0": "110.",
  "anya-e1008n0:548_0": "Kiṃkarissanti vattāro,",
  "anya-e1008n0:549_0": "Sotaṃ yattha navijjate;",
  "anya-e1008n0:550_0": "Naggakapaṇake dese,",
  "anya-e1008n0:551_0": "Rajaṇo kiṃkarissati."
}