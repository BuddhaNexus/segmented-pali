{
  "tha-ap506:0.1_0": "Therāpadāna",
  "tha-ap506:0.2_0": "Kaṇikāravagga",
  "tha-ap506:0.3_0": "4. Avaṭaphaliyattheraapadāna",
  "tha-ap506:1.1_0": "“Sahassaraṁsī bhagavā,",
  "tha-ap506:1.2_0": "sayambhū aparājito;",
  "tha-ap506:1.3_0": "Vivekā vuṭṭhahitvāna,",
  "tha-ap506:1.4_0": "gocarāyābhinikkhami.",
  "tha-ap506:2.1_0": "Phalahattho ahaṁ disvā,",
  "tha-ap506:2.2_0": "upagacchiṁ narāsabhaṁ;",
  "tha-ap506:2.3_0": "Pasannacitto sumano,",
  "tha-ap506:2.4_0": "avaṭaṁ adadiṁ phalaṁ.",
  "tha-ap506:3.1_0": "Catunnavutito kappe,",
  "tha-ap506:3.2_0": "yaṁ phalaṁ adadiṁ tadā;",
  "tha-ap506:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap506:3.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap506:4.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap506:4.2_0": "…pe…",
  "tha-ap506:4.3_0": "viharāmi anāsavo.",
  "tha-ap506:5.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap506:5.2_0": "…pe…",
  "tha-ap506:5.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap506:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap506:6.2_0": "…pe…",
  "tha-ap506:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap506:7.1_0": "Itthaṁ sudaṁ āyasmā avaṭaphaliyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap506:8.1_0": "Avaṭaphaliyattherassāpadānaṁ catutthaṁ."
}