{
  "ne27:0.1_0": "Netti",
  "ne27:0.2_0": "Paṭiniddesavāra",
  "ne27:0.3_0": "Sampāta 8",
  "ne27:0.4_0": "Vibhattihārasampāta",
  "ne27:1.1_0": "Tattha katamo vibhattihārasampāto?",
  "ne27:2.1_0": "“Tasmā rakkhitacittassa, sammāsaṅkappagocaro”ti gāthā.",
  "ne27:2.2_0": "Kusalapakkho kusalapakkhena niddisitabbo.",
  "ne27:2.3_0": "Akusalapakkho akusalapakkhena niddisitabbo.",
  "ne27:2.4_0": "Niyutto vibhattihārasampāto."
}