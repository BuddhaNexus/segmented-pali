{
  "thag1.14:0.1_0": "Theragāthā 1.14",
  "thag1.14:0.2_0": "Ekakanipāta",
  "thag1.14:0.3_0": "Dutiyavagga",
  "thag1.14:0.4_0": "Sivakasāmaṇeragāthā",
  "thag1.14:1.1_0": "“Upajjhāyo maṁ avaca,",
  "thag1.14:1.2_0": "ito gacchāma sīvaka;",
  "thag1.14:1.3_0": "Gāme me vasati kāyo,",
  "thag1.14:1.4_0": "araññaṁ me gato mano;",
  "thag1.14:1.5_0": "Semānakopi gacchāmi,",
  "thag1.14:1.6_0": "natthi saṅgo vijānatan”ti.",
  "thag1.14:1.7_0": "… Vanavacchassa therassa (siviko) sāmaṇero …."
}