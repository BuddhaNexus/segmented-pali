{
  "pli-tv-bu-vb-sk46:0.1_0": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk46:0.2_0": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk46:0.3_0": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk46:0.4_0": "Kabaḷavagga",
  "pli-tv-bu-vb-sk46:0.5_0": "46. Avagaṇḍakārakasikkhāpada",
  "pli-tv-bu-vb-sk46:1.1_0": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk46:1.2_0": "Tena kho pana samayena chabbaggiyā bhikkhū avagaṇḍakārakaṁ bhuñjanti …pe…",
  "pli-tv-bu-vb-sk46:1.3.1_0": "“Na avagaṇḍakārakaṁ bhuñjissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk46:1.4_0": "Na avagaṇḍakārakaṁ bhuñjitabbaṁ.",
  "pli-tv-bu-vb-sk46:1.5_0": "Yo anādariyaṁ paṭicca ekato vā ubhato vā gaṇḍaṁ katvā bhuñjati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk46:1.6.1_0": "Anāpatti—",
  "pli-tv-bu-vb-sk46:1.7_0": "asañcicca,",
  "pli-tv-bu-vb-sk46:1.8_0": "assatiyā,",
  "pli-tv-bu-vb-sk46:1.9_0": "ajānantassa,",
  "pli-tv-bu-vb-sk46:1.10_0": "gilānassa,",
  "pli-tv-bu-vb-sk46:1.11_0": "phalāphale,",
  "pli-tv-bu-vb-sk46:1.12_0": "āpadāsu,",
  "pli-tv-bu-vb-sk46:1.13_0": "ummattakassa,",
  "pli-tv-bu-vb-sk46:1.14_0": "ādikammikassāti.",
  "pli-tv-bu-vb-sk46:1.15_0": "Chaṭṭhasikkhāpadaṁ niṭṭhitaṁ."
}