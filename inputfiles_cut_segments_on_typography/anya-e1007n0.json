{
  "anya-e1007n0:0_0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1007n0:1_0": "Sūrassatīnīti",
  "anya-e1007n0:2_0": "Paṭhamo bhāgo",
  "anya-e1007n0:3_0": "Paṇāmagāthā",
  "anya-e1007n0:4_0": "1.",
  "anya-e1007n0:5_0": "Mukhamhā bhagavantassa,",
  "anya-e1007n0:6_0": "Sugandhakamalā subhā;",
  "anya-e1007n0:7_0": "Sañjātaṃ uttamaṃ vāṇiṃ,",
  "anya-e1007n0:8_0": "Vandāmi vittamānasā.",
  "anya-e1007n0:9_0": "2.",
  "anya-e1007n0:10_0": "Samajjhiṭṭho racissāmi,",
  "anya-e1007n0:11_0": "Dakkhena rājamantinā;",
  "anya-e1007n0:12_0": "Nāmena santena,",
  "anya-e1007n0:13_0": "Nītiṃ lokahitāvahaṃ;",
  "anya-e1007n0:14_0": "Sālaṅkāraṃ sopadesaṃ,",
  "anya-e1007n0:15_0": "Nāmenāhaṃ svarassatiṃ.",
  "anya-e1007n0:16_0": "3.",
  "anya-e1007n0:17_0": "Pekkhantvimaṃ nītiṃ santā,",
  "anya-e1007n0:18_0": "Mañjūsarukkhasannibhaṃ;",
  "anya-e1007n0:19_0": "Nānopadesasaṃpuṇṇaṃ,",
  "anya-e1007n0:20_0": "Sadatthakusalā sadā.",
  "anya-e1007n0:21_0": "Sūrassatīnīti",
  "anya-e1007n0:22_0": "1.",
  "anya-e1007n0:23_0": "Kataññutā ca saccañca,",
  "anya-e1007n0:24_0": "Lokasārā hi te duve;",
  "anya-e1007n0:25_0": "Lokāpi tehi tiṭṭhanti,",
  "anya-e1007n0:26_0": "Raṭṭhaṃ akaṃsu issaraṃ.",
  "anya-e1007n0:27_0": "2.",
  "anya-e1007n0:28_0": "Kākāca dujjanā loke,",
  "anya-e1007n0:29_0": "Malībhūtāva sabbadā;",
  "anya-e1007n0:30_0": "Iṭṭhaṃ guṇaṃ nāsayanti,",
  "anya-e1007n0:31_0": "Te ve lokassa verino.",
  "anya-e1007n0:32_0": "Migānaṃ siṅgālo anto,",
  "anya-e1007n0:33_0": "Pakkhīnaṃ pana vāyaso.",
  "anya-e1007n0:34_0": "Akāraṇaverī honti,",
  "anya-e1007n0:35_0": "Macchānaṃ dhīvarā yathā;",
  "anya-e1007n0:36_0": "Guṇīnaṃ sajjanānañca,",
  "anya-e1007n0:37_0": "Dujjanā niccaverino.",
  "anya-e1007n0:38_0": "3.",
  "anya-e1007n0:39_0": "Siriṃ bhonto suposetha,",
  "anya-e1007n0:40_0": "Siri mūlā hi sampadā;",
  "anya-e1007n0:41_0": "Siriya idha jotanti,",
  "anya-e1007n0:42_0": "Sirī sā sabbasiddhikā.",
  "anya-e1007n0:43_0": "4.",
  "anya-e1007n0:44_0": "Musā tamokarā loke,",
  "anya-e1007n0:45_0": "Saccaṃ mīditikārakaṃ;",
  "anya-e1007n0:46_0": "Musātamena dukkhanti,",
  "anya-e1007n0:47_0": "Saccābhāya sukhantive.",
  "anya-e1007n0:48_0": "Suvijānaṃ siṅgālānaṃ,",
  "anya-e1007n0:49_0": "Sakuṇānañca vassitaṃ;",
  "anya-e1007n0:50_0": "Manussavassitaṃ rāja,",
  "anya-e1007n0:51_0": "Dubbijānataraṃ tato.",
  "anya-e1007n0:52_0": "5.",
  "anya-e1007n0:53_0": "Kappaggisadisā issā,",
  "anya-e1007n0:54_0": "Jhāpeti sabbasampadaṃ;",
  "anya-e1007n0:55_0": "Muditā kappameghova,",
  "anya-e1007n0:56_0": "Ropeti sabbasampadaṃ.",
  "anya-e1007n0:57_0": "6.",
  "anya-e1007n0:58_0": "Yathā asanthirā thambhā,",
  "anya-e1007n0:59_0": "Thusarāsimhi ussitā;",
  "anya-e1007n0:60_0": "Tatheva kapicittānaṃ,",
  "anya-e1007n0:61_0": "Kammantā cañcalaṅgatā.",
  "anya-e1007n0:62_0": "Haliddirāgaṃ kapicittaṃ;",
  "anya-e1007n0:63_0": "Purisaṃ rāgavirāginaṃ,",
  "anya-e1007n0:64_0": "Edisaṃ tāta māsevi;",
  "anya-e1007n0:65_0": "Nimmanussampi ce siyā.",
  "anya-e1007n0:66_0": "7. [Ka]",
  "anya-e1007n0:67_0": "Pupphacumbi cittapattī,",
  "anya-e1007n0:68_0": "Sakavaṇṇena majjito;",
  "anya-e1007n0:69_0": "Vikkamī pupphato pupphaṃ,",
  "anya-e1007n0:70_0": "Nassāgāraṃ na saṅgamo.",
  "anya-e1007n0:71_0": "[Kha]",
  "anya-e1007n0:72_0": "Kippilikā dubbaṇṇāpi,",
  "anya-e1007n0:73_0": "Samaggāca parakkamā;",
  "anya-e1007n0:74_0": "Mā hotha pupphacumbīva,",
  "anya-e1007n0:75_0": "Hotha vo pacikā yathā.",
  "anya-e1007n0:76_0": "1.",
  "anya-e1007n0:77_0": "Ye maranti kīḷantā te,",
  "anya-e1007n0:78_0": "Svānā aññoññamoditā;",
  "anya-e1007n0:79_0": "Disvāna chaṭṭitaṃ bhattaṃ,",
  "anya-e1007n0:80_0": "Sīghaverī vihiṃsare.",
  "anya-e1007n0:81_0": "2.",
  "anya-e1007n0:82_0": "Tathekepi janā dāni,",
  "anya-e1007n0:83_0": "Samaggā ññoññamoditā;",
  "anya-e1007n0:84_0": "Dhanahetu vihiṃsanti,",
  "anya-e1007n0:85_0": "Dhīratthu sīghaverikā.",
  "anya-e1007n0:86_0": "9.",
  "anya-e1007n0:87_0": "Vasante hemante gimhe;",
  "anya-e1007n0:88_0": "Neva tālā visesino;",
  "anya-e1007n0:89_0": "Thiracittā janā santā;",
  "anya-e1007n0:90_0": "Sukhadukkhesu niccalā.",
  "anya-e1007n0:91_0": "10.",
  "anya-e1007n0:92_0": "Yathā pavaṭṭamānamhi,",
  "anya-e1007n0:93_0": "Suṭṭhu tiṭṭhati geṇṭhuke;",
  "anya-e1007n0:94_0": "Appavaṭṭe bhūmyaṃ seti,",
  "anya-e1007n0:95_0": "Tatheva geṇṭhuko jano.",
  "anya-e1007n0:96_0": "11.",
  "anya-e1007n0:97_0": "Agghāpetuṃ nasakkonti,",
  "anya-e1007n0:98_0": "Kālañhi kālikā janā;",
  "anya-e1007n0:99_0": "Vajirādiñca sakkonti,",
  "anya-e1007n0:100_0": "Tena kālo anagghiko.",
  "anya-e1007n0:101_0": "12. [Ka]",
  "anya-e1007n0:102_0": "Asano hi dīghaddhāno,",
  "anya-e1007n0:103_0": "Sārasāro sugandhiko;",
  "anya-e1007n0:104_0": "Niggandho tveva nissāro,",
  "anya-e1007n0:105_0": "Dīghaddhānopi simbalī.",
  "anya-e1007n0:106_0": "[Kha]",
  "anya-e1007n0:107_0": "Tatheveke janā loke,",
  "anya-e1007n0:108_0": "Dīghaddhānā susārakā;",
  "anya-e1007n0:109_0": "Nissārā keci pheggūva,",
  "anya-e1007n0:110_0": "Dīghaddhānāpi goyathā.",
  "anya-e1007n0:111_0": "13.",
  "anya-e1007n0:112_0": "Upakāro cāpakāro,",
  "anya-e1007n0:113_0": "Yasmiṃ gacchati naṭṭhataṃ;",
  "anya-e1007n0:114_0": "Pāsāṇahadayassassa,",
  "anya-e1007n0:115_0": "Jīvatītyā bhidhāmudhā.",
  "anya-e1007n0:116_0": "Pasādo nipphalo yassa,",
  "anya-e1007n0:117_0": "Kopocāpi niratthako;",
  "anya-e1007n0:118_0": "Na taṃ saṅgantu miccheyya,",
  "anya-e1007n0:119_0": "Thīpumāva napuṃsakaṃ.",
  "anya-e1007n0:120_0": "14.",
  "anya-e1007n0:121_0": "Upacāro hi kātabbo,",
  "anya-e1007n0:122_0": "Na yāva sohadaṃ bhave;",
  "anya-e1007n0:123_0": "Upacāro sumittamhi,",
  "anya-e1007n0:124_0": "Māyā ca hoti koṭilaṃ.",
  "anya-e1007n0:125_0": "15.",
  "anya-e1007n0:126_0": "Kamena aggato ucchu,",
  "anya-e1007n0:127_0": "Raso sādutaro yathā;",
  "anya-e1007n0:128_0": "Tatheva sumitto loke,",
  "anya-e1007n0:129_0": "Dummitto pana nediso.",
  "anya-e1007n0:130_0": "16.",
  "anya-e1007n0:131_0": "Sokārāti parittāṇaṃ,",
  "anya-e1007n0:132_0": "Vissāsapītibhājanaṃ;",
  "anya-e1007n0:133_0": "Ratanābhiratanaṃ icche,",
  "anya-e1007n0:134_0": "Sumittaṃ akkharattayaṃ.",
  "anya-e1007n0:135_0": "17.",
  "anya-e1007n0:136_0": "Dampatīnaṃ sumittānaṃ,",
  "anya-e1007n0:137_0": "Mukhaṃ aññoññadappaṇaṃ,",
  "anya-e1007n0:138_0": "Sukhe sukhaṃ dukkhe dukkhaṃ,",
  "anya-e1007n0:139_0": "Paṭicchāyeva dappaṇe.",
  "anya-e1007n0:140_0": "Kathaṃ nu tāsaṃ hadayaṃ,",
  "anya-e1007n0:141_0": "Sukharāvata itthiyo;",
  "anya-e1007n0:142_0": "Yāsāmike dukkhitamhi,",
  "anya-e1007n0:143_0": "Sukhamicchanti attano.",
  "anya-e1007n0:144_0": "18.",
  "anya-e1007n0:145_0": "Nivātañca pure katvā,",
  "anya-e1007n0:146_0": "Mānaṃ katvāna pacchato;",
  "anya-e1007n0:147_0": "Sakatthaṃ dhāraye dhīro,",
  "anya-e1007n0:148_0": "Atthabhañjo hi muḷhatā.",
  "anya-e1007n0:149_0": "19.",
  "anya-e1007n0:150_0": "Purecāraṃ satiṃ katvā,",
  "anya-e1007n0:151_0": "Saddhaṃ kareyya pacchato;",
  "anya-e1007n0:152_0": "Turaṃ na saddahe dhīro,",
  "anya-e1007n0:153_0": "Sīghasaddho hi mandako.",
  "anya-e1007n0:154_0": "20.",
  "anya-e1007n0:155_0": "Vane 0.0099 bahūni kaṭṭhāni;",
  "anya-e1007n0:156_0": "Dullabhaṃ rattacandanaṃ;",
  "anya-e1007n0:157_0": "Tathā janā bahū loke;",
  "anya-e1007n0:158_0": "Pumā jañño sudullabho.",
  "anya-e1007n0:159_0": "21.",
  "anya-e1007n0:160_0": "Tiṇakaṭṭhapalāsehi;",
  "anya-e1007n0:161_0": "Sukkhehi dayhate vanaṃ;",
  "anya-e1007n0:162_0": "Etādīhi asārehi;",
  "anya-e1007n0:163_0": "Loko janehi dayhate.",
  "anya-e1007n0:164_0": "22.",
  "anya-e1007n0:165_0": "Antovasse timāsamhi,",
  "anya-e1007n0:166_0": "Puññakammena moditā;",
  "anya-e1007n0:167_0": "Sukhaṃ vasiṃsu porāṇā,",
  "anya-e1007n0:168_0": "Buddhasāsanamāmakā.",
  "anya-e1007n0:169_0": "23.",
  "anya-e1007n0:170_0": "Migamadena ekena,",
  "anya-e1007n0:171_0": "Taṃ vanaṃ surabhigandhikaṃ;",
  "anya-e1007n0:172_0": "Tathā janena taṃ raṭṭhaṃ,",
  "anya-e1007n0:173_0": "Guṇinā hi sirīmatā.",
  "anya-e1007n0:174_0": "Sarīraṃ khaṇaviddhaṃsī,",
  "anya-e1007n0:175_0": "Kappantaṭṭhāyino guṇā.",
  "anya-e1007n0:176_0": "24.",
  "anya-e1007n0:177_0": "Kharānaṃ sīhabyagghānaṃ,",
  "anya-e1007n0:178_0": "Saṅgamo no hisabbadā;",
  "anya-e1007n0:179_0": "Tatheva byagghacittānaṃ,",
  "anya-e1007n0:180_0": "Sajātikā khayonatā.",
  "anya-e1007n0:181_0": "25.",
  "anya-e1007n0:182_0": "Saka sādhupi no sādhū,",
  "anya-e1007n0:183_0": "Yo ceñña duṭṭhakārako;",
  "anya-e1007n0:184_0": "Bahūnaṃ sādhū pāyena,",
  "anya-e1007n0:185_0": "Sa ve sādhūti vuccate.",
  "anya-e1007n0:186_0": "26.",
  "anya-e1007n0:187_0": "Bahūdake samuddepi,",
  "anya-e1007n0:188_0": "Jalaṃ nattheva pātave;",
  "anya-e1007n0:189_0": "Khuddake khatakūpamhi,",
  "anya-e1007n0:190_0": "Sāduṃ atthi bahuṃ dakaṃ.",
  "anya-e1007n0:191_0": "27.",
  "anya-e1007n0:192_0": "Mā sīghaṃ vivareyyātha,",
  "anya-e1007n0:193_0": "Nindituñca pasaṃsituṃ;",
  "anya-e1007n0:194_0": "Mukhañhi vo kathādvāraṃ,",
  "anya-e1007n0:195_0": "Nirundheyyātha sabbadā.",
  "anya-e1007n0:196_0": "28.",
  "anya-e1007n0:197_0": "Mā sīghaṃ vivareyyātha,",
  "anya-e1007n0:198_0": "Cakkhuṃ vo dassituṃ piyaṃ;",
  "anya-e1007n0:199_0": "Saṇikañhi piyalābhaṃ,",
  "anya-e1007n0:200_0": "Dhanalābhaṃ turaṃ kare.",
  "anya-e1007n0:201_0": "29.",
  "anya-e1007n0:202_0": "Anārambho hi kammānaṃ,",
  "anya-e1007n0:203_0": "Paṭhamaṃ buddhilakkhaṇaṃ;",
  "anya-e1007n0:204_0": "Niṭṭhaṅgataṃ āraddhassa,",
  "anya-e1007n0:205_0": "Dutiyaṃ buddhilakkhaṇaṃ.",
  "anya-e1007n0:206_0": "Asamekkhitakammantaṃ,",
  "anya-e1007n0:207_0": "Turitābhinipātinaṃ;",
  "anya-e1007n0:208_0": "Sāni kammāni tappenti,",
  "anya-e1007n0:209_0": "Uṇhaṃ vajjhohaṭaṃ mukhe.",
  "anya-e1007n0:210_0": "30.",
  "anya-e1007n0:211_0": "Aphalāni durantāni,",
  "anya-e1007n0:212_0": "Janatā ninditāni ca;",
  "anya-e1007n0:213_0": "Asakyāni ca kammāni,",
  "anya-e1007n0:214_0": "Nārabhetha vicakkhaṇo.",
  "anya-e1007n0:215_0": "31.",
  "anya-e1007n0:216_0": "Ativirodhabhītānaṃ,",
  "anya-e1007n0:217_0": "Saṅkitānaṃ pade pade;",
  "anya-e1007n0:218_0": "Parappavādatāsānaṃ,",
  "anya-e1007n0:219_0": "Dūrato yanti sampadā.",
  "anya-e1007n0:220_0": "Saddamattaṃ na bhetabbaṃ,",
  "anya-e1007n0:221_0": "Loko saddassa gocaro;",
  "anya-e1007n0:222_0": "Yo ca saddaparittāso,",
  "anya-e1007n0:223_0": "Vane bhantamigo hi so.",
  "anya-e1007n0:224_0": "32.",
  "anya-e1007n0:225_0": "Dvinnaṃ taṇḍulathūsānaṃ,",
  "anya-e1007n0:226_0": "Viseso suṭṭhu khāyati;",
  "anya-e1007n0:227_0": "Randhitopi siniddho no,",
  "anya-e1007n0:228_0": "Thuso virasaphāruso;",
  "anya-e1007n0:229_0": "Taṇḍulaṃ siniddhaṃ rasaṃ,",
  "anya-e1007n0:230_0": "Evaṃ lokepi ñāyate.",
  "anya-e1007n0:231_0": "33.",
  "anya-e1007n0:232_0": "Eraṇḍaṃ nissitā valli,",
  "anya-e1007n0:233_0": "Ruhate kiṃ yathābalaṃ;",
  "anya-e1007n0:234_0": "Mahāsālaṃ sunissāya,",
  "anya-e1007n0:235_0": "Ruhate brahataṃ gatā.",
  "anya-e1007n0:236_0": "34.",
  "anya-e1007n0:237_0": "Mettā hi sīmasambhedā,",
  "anya-e1007n0:238_0": "Pakkhapāta vighātikā;",
  "anya-e1007n0:239_0": "Pakkhapātena dukkhanti,",
  "anya-e1007n0:240_0": "Nippakkho vasate sukhaṃ.",
  "anya-e1007n0:241_0": "35.",
  "anya-e1007n0:242_0": "Narā paññā ca laṅkārā,",
  "anya-e1007n0:243_0": "Yathāṭhāne niyujjare;",
  "anya-e1007n0:244_0": "No hi cūḷāmaṇi pāde,",
  "anya-e1007n0:245_0": "Pādukā ca siropari.",
  "anya-e1007n0:246_0": "Ukkuṭṭhe sūra micchanti;",
  "anya-e1007n0:247_0": "Mantīsu akutūhalaṃ;",
  "anya-e1007n0:248_0": "Viyañca annapānamhi;",
  "anya-e1007n0:249_0": "Atthe jāte ca paṇḍitaṃ.",
  "anya-e1007n0:250_0": "36.",
  "anya-e1007n0:251_0": "Pamādo hi tamo loke,",
  "anya-e1007n0:252_0": "Kālo coro bhayānako;",
  "anya-e1007n0:253_0": "Kāyagehaṃ bahuchiddaṃ,",
  "anya-e1007n0:254_0": "Kālacorassa coritaṃ.",
  "anya-e1007n0:255_0": "37.",
  "anya-e1007n0:256_0": "Cañcalo kāladāso hi,",
  "anya-e1007n0:257_0": "Dhītimā kālaissaro;",
  "anya-e1007n0:258_0": "Kālissaro raṭṭhissaraṃ,",
  "anya-e1007n0:259_0": "Ativattati sabbaso.",
  "anya-e1007n0:260_0": "38.",
  "anya-e1007n0:261_0": "Viluppanti 8 dhanaṃ eke,",
  "anya-e1007n0:262_0": "Kālameke anekkhakā;",
  "anya-e1007n0:263_0": "Tesu kālavilopāva,",
  "anya-e1007n0:264_0": "Bhayānakā tikakkhaḷā.",
  "anya-e1007n0:265_0": "39.",
  "anya-e1007n0:266_0": "Raññā raṭṭhahitaṃ kattā,",
  "anya-e1007n0:267_0": "Rañño hitaṃ janehi ve;",
  "anya-e1007n0:268_0": "Desso attahitaṃ dassī,",
  "anya-e1007n0:269_0": "Gārayho kinnu kārako.",
  "anya-e1007n0:270_0": "Attadatthaṃ paratthena;",
  "anya-e1007n0:271_0": "Bahunāpi na hāpaye.",
  "anya-e1007n0:272_0": "40.",
  "anya-e1007n0:273_0": "Yassa upakāro dinno,",
  "anya-e1007n0:274_0": "Upakāraṃ dade puna;",
  "anya-e1007n0:275_0": "Tato pakāraṃ niccheyya,",
  "anya-e1007n0:276_0": "Kataññū dullabho idha.",
  "anya-e1007n0:277_0": "Saccaṃ kireva māhaṃsu,",
  "anya-e1007n0:278_0": "Narā ekacciyā idha;",
  "anya-e1007n0:279_0": "Kaṭṭhaṃ niplavitaṃ seyyo,",
  "anya-e1007n0:280_0": "Natveve kacciyo naro.",
  "anya-e1007n0:281_0": "41.",
  "anya-e1007n0:282_0": "Kāruko sakapaññāya,",
  "anya-e1007n0:283_0": "Mahagghaṃ dārukaṃ kare;",
  "anya-e1007n0:284_0": "Tathā janopi attānaṃ,",
  "anya-e1007n0:285_0": "Mahaggho lokamānito.",
  "anya-e1007n0:286_0": "42.",
  "anya-e1007n0:287_0": "Appaggho hi ayo hemaṃ,",
  "anya-e1007n0:288_0": "Mahagghaṃ chindate yathā;",
  "anya-e1007n0:289_0": "Nigguṇo saguṇaṃ loke,",
  "anya-e1007n0:290_0": "Alakkhīca siriṃ tathā.",
  "anya-e1007n0:291_0": "Sūrassatīnīti",
  "anya-e1007n0:292_0": "Dutiyo bhāgo",
  "anya-e1007n0:293_0": "1.",
  "anya-e1007n0:294_0": "Dhanassa dubbidhaṃ kiccaṃ,",
  "anya-e1007n0:295_0": "Pāpeti uṇṇataṃ dhaniṃ;",
  "anya-e1007n0:296_0": "Adhaniṃ oṇataṃ loke,",
  "anya-e1007n0:297_0": "Sañcine tena taṃ dhanaṃ.",
  "anya-e1007n0:298_0": "2.",
  "anya-e1007n0:299_0": "Vaṭṭate satataṃ sīghaṃ,",
  "anya-e1007n0:300_0": "Kālacakkaṃ avāritaṃ;",
  "anya-e1007n0:301_0": "Tena ghaṭī dinaṃ māso,",
  "anya-e1007n0:302_0": "Vasso bhavatya cīrato.",
  "anya-e1007n0:303_0": "3.",
  "anya-e1007n0:304_0": "Sattunā na hi sandheyya,",
  "anya-e1007n0:305_0": "Ekadā so bhayaṃ karo;",
  "anya-e1007n0:306_0": "Sutattamapi pānīyaṃ,",
  "anya-e1007n0:307_0": "Samayate nu pāvakaṃ.",
  "anya-e1007n0:308_0": "4.",
  "anya-e1007n0:309_0": "Vijahaṃ pakatiṃ yo hi,",
  "anya-e1007n0:310_0": "Vikatiṃ puna gacchati;",
  "anya-e1007n0:311_0": "Sabhāvena ākārena,",
  "anya-e1007n0:312_0": "Vippallāsaṃ sa gacchati;",
  "anya-e1007n0:313_0": "Saṃsumāra gatā godhā,",
  "anya-e1007n0:314_0": "Yathā thī pumavesikā.",
  "anya-e1007n0:315_0": "5.",
  "anya-e1007n0:316_0": "Pakkhaṃ laddhāna uḍḍenti,",
  "anya-e1007n0:317_0": "Upacikā hi vammikā;",
  "anya-e1007n0:318_0": "Nikkhantā maraṇaṃ yanti,",
  "anya-e1007n0:319_0": "Uppatā nippataṃ gatā.",
  "anya-e1007n0:320_0": "6.",
  "anya-e1007n0:321_0": "Sabbaṃpiyassa dajjeyya,",
  "anya-e1007n0:322_0": "Nissesaṃ piyamānasaṃ;",
  "anya-e1007n0:323_0": "Saddhācittaṃ tu no viññū,",
  "anya-e1007n0:324_0": "Saddhāyiko pakkhalito.",
  "anya-e1007n0:325_0": "7.",
  "anya-e1007n0:326_0": "Sakkoti laṅghituṃ byāmaṃ,",
  "anya-e1007n0:327_0": "Mahussāhena yo hi so;",
  "anya-e1007n0:328_0": "Tadaḍḍhaṃ anussāhena,",
  "anya-e1007n0:329_0": "Nossāho tesu thomito;",
  "anya-e1007n0:330_0": "Mahussāho dukkho loke,",
  "anya-e1007n0:331_0": "Anussāho sadā sukho.",
  "anya-e1007n0:332_0": "8.",
  "anya-e1007n0:333_0": "Dukkhamaṃ akkhamanto yo,",
  "anya-e1007n0:334_0": "Piṭṭhikārīca dukkaraṃ,",
  "anya-e1007n0:335_0": "Kadā labheyya so loke,",
  "anya-e1007n0:336_0": "Sukhamaṃ sukaraṃ mudhā;",
  "anya-e1007n0:337_0": "Paccakkhañhi so kareyya,",
  "anya-e1007n0:338_0": "Dukkhamañcāpi dukkaraṃ.",
  "anya-e1007n0:339_0": "9.",
  "anya-e1007n0:340_0": "Anaggho manusso loke,",
  "anya-e1007n0:341_0": "Tosanāposanādinā;",
  "anya-e1007n0:342_0": "Tena so mahagghaṃ kammaṃ,",
  "anya-e1007n0:343_0": "Kare lokahitāyutaṃ.",
  "anya-e1007n0:344_0": "10.",
  "anya-e1007n0:345_0": "Vātena nappabhijjanti,",
  "anya-e1007n0:346_0": "Ninnā veḷū kasā naḷā;",
  "anya-e1007n0:347_0": "Yathāvātaṃ nagacchanti,",
  "anya-e1007n0:348_0": "Tathā care jane kadā.",
  "anya-e1007n0:349_0": "11.",
  "anya-e1007n0:350_0": "Purato ca pacchato ce,",
  "anya-e1007n0:351_0": "Nissayo natthi passato;",
  "anya-e1007n0:352_0": "Adhikaṃ vīriyaṃ hoti,",
  "anya-e1007n0:353_0": "Attanātho tadā bhave.",
  "anya-e1007n0:354_0": "12.",
  "anya-e1007n0:355_0": "Khaṇaṃ ākhubilaṃ sīho,",
  "anya-e1007n0:356_0": "Pāsāṇasakalākulaṃ;",
  "anya-e1007n0:357_0": "Pappoti nakhabhaṅgaṃvā,",
  "anya-e1007n0:358_0": "Phalaṃvā mūsiko bhave.",
  "anya-e1007n0:359_0": "13.",
  "anya-e1007n0:360_0": "Maggamuḷhā janā andhā,",
  "anya-e1007n0:361_0": "Amaggā maggasaññino;",
  "anya-e1007n0:362_0": "Tatheva duppaññā muḷhā,",
  "anya-e1007n0:363_0": "Tathatthaṃ nāva bujjhare.",
  "anya-e1007n0:364_0": "14.",
  "anya-e1007n0:365_0": "Piyarūpaṃ vīrarūpaṃ,",
  "anya-e1007n0:366_0": "Duvidhā rūpasampadā,",
  "anya-e1007n0:367_0": "Nāriṃ icche piyarūpiṃ,",
  "anya-e1007n0:368_0": "Purisaṃ vīrarūpakaṃ.",
  "anya-e1007n0:369_0": "15.",
  "anya-e1007n0:370_0": "Pajjalanti hi khajjotā,",
  "anya-e1007n0:371_0": "Pakkhacālanakammunā;",
  "anya-e1007n0:372_0": "Kusitā supitā nete,",
  "anya-e1007n0:373_0": "Tathā janāpi kammikā.",
  "anya-e1007n0:374_0": "16.",
  "anya-e1007n0:375_0": "Kummasaṅkocamopamya,",
  "anya-e1007n0:376_0": "Niggahamapi saṃkhame;",
  "anya-e1007n0:377_0": "Pattakāle tu nītiñño,",
  "anya-e1007n0:378_0": "Kaṇhasappova uṭṭhahe.",
  "anya-e1007n0:379_0": "17.",
  "anya-e1007n0:380_0": "Bhakkhasesaṃ nakhādanti,",
  "anya-e1007n0:381_0": "Sīhā unnatacetasā;",
  "anya-e1007n0:382_0": "Paraṃpi napaṇāmenti,",
  "anya-e1007n0:383_0": "Vuddhikāmā tathā care.",
  "anya-e1007n0:384_0": "18.",
  "anya-e1007n0:385_0": "Na va vasso samuppanno,",
  "anya-e1007n0:386_0": "Khīṇo purāṇahāyano;",
  "anya-e1007n0:387_0": "Navavasse navā mettā,",
  "anya-e1007n0:388_0": "Bhāvitabbā hitesinā.",
  "anya-e1007n0:389_0": "19.",
  "anya-e1007n0:390_0": "Santāpayanti kamayāpyabhujaṃ na rogā,",
  "anya-e1007n0:391_0": "Dummantinaṃ kamupayanti na nītidosā;",
  "anya-e1007n0:392_0": "Kaṃ srī na mānayati kaṃ na ca hanti maccu,",
  "anya-e1007n0:393_0": "Kaṃ thīkatā na visayā paripīḷayanti.",
  "anya-e1007n0:394_0": "(Vasantatilakāgāthā.)",
  "anya-e1007n0:395_0": "20.",
  "anya-e1007n0:396_0": "(Ka) byāmamattena daṇḍena,",
  "anya-e1007n0:397_0": "Yolumbya udakaṃ mine;",
  "anya-e1007n0:398_0": "Agambhīraṃ gambhīraṃvā,",
  "anya-e1007n0:399_0": "Agādhe maññi gambhīraṃ.",
  "anya-e1007n0:400_0": "(Kha)",
  "anya-e1007n0:401_0": "Tathā mando saññāṇena,",
  "anya-e1007n0:402_0": "Agādhe maññi paṇḍitaṃ;",
  "anya-e1007n0:403_0": "Samāsamaṃ na jānāti,",
  "anya-e1007n0:404_0": "Bahvappaṃ tikkhamandataṃ.",
  "anya-e1007n0:405_0": "21.",
  "anya-e1007n0:406_0": "Duṭṭhakamme saṅgamanti,",
  "anya-e1007n0:407_0": "Chekakamme ca no idha;",
  "anya-e1007n0:408_0": "Maccuṃ vahanti sīsena,",
  "anya-e1007n0:409_0": "Te muḷhā muḷhasaṅgamā.",
  "anya-e1007n0:410_0": "22.",
  "anya-e1007n0:411_0": "Duvidho saṅgamo loke,",
  "anya-e1007n0:412_0": "Ujuko kuṭilo bhave;",
  "anya-e1007n0:413_0": "Ujukova pasaṃseyyo,",
  "anya-e1007n0:414_0": "Nohyañño sājasaṅgamo.",
  "anya-e1007n0:415_0": "Te iminā upāyena samaggā sammodamānā mahā bhittipiṭṭhikāya vasanti. (Mahosadhajātaka aṭṭhakathā)",
  "anya-e1007n0:416_0": "23.",
  "anya-e1007n0:417_0": "Yūthikā pupphate nohi,",
  "anya-e1007n0:418_0": "Siñcitāpi punappunaṃ;",
  "anya-e1007n0:419_0": "Pupphate sampatte kāle,",
  "anya-e1007n0:420_0": "Evaṃ dhāretha vīriyaṃ.",
  "anya-e1007n0:421_0": "24.",
  "anya-e1007n0:422_0": "Dhanuccayo dhanakkhepo,",
  "anya-e1007n0:423_0": "Duvidhā hi dhanākati;",
  "anya-e1007n0:424_0": "Dhanuccaye nayo atthi,",
  "anya-e1007n0:425_0": "Dhanakkhepamhi no idha.",
  "anya-e1007n0:426_0": "25.",
  "anya-e1007n0:427_0": "Amātā pitarasaṃ vaḍḍhaṃ,",
  "anya-e1007n0:428_0": "Jūtakārañca cañcalaṃ;",
  "anya-e1007n0:429_0": "Nālapeyya visesaññū,",
  "anya-e1007n0:430_0": "Yadicche siddhi mattano.",
  "anya-e1007n0:431_0": "Haliddirāgaṃ kapicittaṃ,",
  "anya-e1007n0:432_0": "Purisaṃ rāgavirāginaṃ;",
  "anya-e1007n0:433_0": "Edisaṃ tāta māsevi,",
  "anya-e1007n0:434_0": "Nimmanussaṃpi ce siyā.",
  "anya-e1007n0:435_0": "26.",
  "anya-e1007n0:436_0": "Guṇā guṇaññūsu guṇā bhavanti,",
  "anya-e1007n0:437_0": "Te nigguṇaṃ patvā bhavanti dosā;",
  "anya-e1007n0:438_0": "Āsādyatoyā pabhavanti najjo,",
  "anya-e1007n0:439_0": "Samudramāsajja bhavantyapeyyā.",
  "anya-e1007n0:440_0": "(Upajātigāthā)",
  "anya-e1007n0:441_0": "27.",
  "anya-e1007n0:442_0": "Silārūpaṃ nimminanti,",
  "anya-e1007n0:443_0": "Koṭṭetvāna punappunaṃ;",
  "anya-e1007n0:444_0": "Koṭṭakova tathā bālā,",
  "anya-e1007n0:445_0": "Sādhuṃ ovajja nimmitā.",
  "anya-e1007n0:446_0": "Cāṇakyanītilā gāthā",
  "anya-e1007n0:447_0": "Lālane bahavo dosā,",
  "anya-e1007n0:448_0": "Tāḷane bahavo guṇā;",
  "anya-e1007n0:449_0": "Tasmā puttañca sissañca,",
  "anya-e1007n0:450_0": "Tāḷaye na ca lālaye.",
  "anya-e1007n0:451_0": "28.",
  "anya-e1007n0:452_0": "Atītassa hi mittassa,",
  "anya-e1007n0:453_0": "Yo ce dosaṃ pakāsaye;",
  "anya-e1007n0:454_0": "So have paccuppannassa,",
  "anya-e1007n0:455_0": "Dosaṃ bhāseti ñāyati.",
  "anya-e1007n0:456_0": "29.",
  "anya-e1007n0:457_0": "Latāviya sevakā te,",
  "anya-e1007n0:458_0": "Ye nissayaṃ palambare;",
  "anya-e1007n0:459_0": "Nissayassa vināsena,",
  "anya-e1007n0:460_0": "Bhūmyaṃ senti anāthakā.",
  "anya-e1007n0:461_0": "30.",
  "anya-e1007n0:462_0": "Dosasiṅgehi vijjhanto,",
  "anya-e1007n0:463_0": "Mānakhūrehi akkamaṃ;",
  "anya-e1007n0:464_0": "Bhayaṃ karoti lokamhi,",
  "anya-e1007n0:465_0": "Gova bālo vihiṃsako.",
  "anya-e1007n0:466_0": "31.",
  "anya-e1007n0:467_0": "Ādo upari lokoyaṃ,",
  "anya-e1007n0:468_0": "Ujulekhāya tiṭṭhati;",
  "anya-e1007n0:469_0": "Musāvātehi taṃlokaṃ,",
  "anya-e1007n0:470_0": "Nipātesi anajjavaṃ.",
  "anya-e1007n0:471_0": "32.",
  "anya-e1007n0:472_0": "Sughaṭaṃ kumbhakārena,",
  "anya-e1007n0:473_0": "Nāraho paribhuñjituṃ;",
  "anya-e1007n0:474_0": "Tathūpamāya vekkheyya,",
  "anya-e1007n0:475_0": "Sakammaparakammani.",
  "anya-e1007n0:476_0": "33.",
  "anya-e1007n0:477_0": "Anantaraṃsī sūropi,",
  "anya-e1007n0:478_0": "Nasakkoti ghanaṃ tamaṃ;",
  "anya-e1007n0:479_0": "Vijjhituṃ raṃsiyā loke,",
  "anya-e1007n0:480_0": "Tathā madanamohitā;",
  "anya-e1007n0:481_0": "Nasakkonti madaṃ bhetvā,",
  "anya-e1007n0:482_0": "Paññābhāya pabhāsituṃ.",
  "anya-e1007n0:483_0": "34.",
  "anya-e1007n0:484_0": "Khedaveraṃ daliddamhi,",
  "anya-e1007n0:485_0": "Bhogimhi rogupaddavaṃ;",
  "anya-e1007n0:486_0": "Dessaverañca āṇimhi,",
  "anya-e1007n0:487_0": "Passe lokassa veritaṃ.",
  "anya-e1007n0:488_0": "35.",
  "anya-e1007n0:489_0": "Saṃladdhena subhogena,",
  "anya-e1007n0:490_0": "Jīvaṃ suddhaṃ kare nijaṃ;",
  "anya-e1007n0:491_0": "Seṭṭho so tena jīvena,",
  "anya-e1007n0:492_0": "Jeguccho malajīviko.",
  "anya-e1007n0:493_0": "36.",
  "anya-e1007n0:494_0": "Vajira puppharāgānaṃ,",
  "anya-e1007n0:495_0": "Visesaṃ yo nabujjhati;",
  "anya-e1007n0:496_0": "Kathañhi so vikkīṇeyya,",
  "anya-e1007n0:497_0": "Kīṇeyya vā yathātathaṃ.",
  "anya-e1007n0:498_0": "37.",
  "anya-e1007n0:499_0": "Kippīli kopi cintetvā,",
  "anya-e1007n0:500_0": "Pabbataṃ bhettu mussahaṃ;",
  "anya-e1007n0:501_0": "Abalā tanumajjhattā,",
  "anya-e1007n0:502_0": "Cintā hasyāva sā mudhā.",
  "anya-e1007n0:503_0": "38.",
  "anya-e1007n0:504_0": "Jātamattaṃ na yo sattuṃ,",
  "anya-e1007n0:505_0": "Rogañcūpasamaṃ naye;",
  "anya-e1007n0:506_0": "Mahābalopi teneva,",
  "anya-e1007n0:507_0": "Vuddhiṃpatvā sa haññate.",
  "anya-e1007n0:508_0": "39.",
  "anya-e1007n0:509_0": "Sajīvamaṃsabhakkhehi,",
  "anya-e1007n0:510_0": "Sadāṭhīhi mukhehi bho;",
  "anya-e1007n0:511_0": "Biḷārabyagghasīhānaṃ,",
  "anya-e1007n0:512_0": "Nihīnāni anekadhā;",
  "anya-e1007n0:513_0": "Tikkhāni kharavādāni,",
  "anya-e1007n0:514_0": "Manussānaṃ mukhāni ve.",
  "anya-e1007n0:515_0": "40.",
  "anya-e1007n0:516_0": "Viluppantā vidhāvanti,",
  "anya-e1007n0:517_0": "Sajīvavuttikammunā;",
  "anya-e1007n0:518_0": "Janā tena vihaññanti,",
  "anya-e1007n0:519_0": "Caranti dhammavemukhā.",
  "anya-e1007n0:520_0": "41.",
  "anya-e1007n0:521_0": "Sulabhaṃ lokiyaṃ loke,",
  "anya-e1007n0:522_0": "Sāsanīyaṃva dullabhaṃ;",
  "anya-e1007n0:523_0": "Dullabhaṃ taṃ vamaññanto,",
  "anya-e1007n0:524_0": "Eso bālatamo bhave.",
  "anya-e1007n0:525_0": "42.",
  "anya-e1007n0:526_0": "Yo patittha agyāvāṭaṃ,",
  "anya-e1007n0:527_0": "Mohā taṃ upakārituṃ;",
  "anya-e1007n0:528_0": "Aññorohi tadā vāṭaṃ,",
  "anya-e1007n0:529_0": "Dutīyo muḷhamuḷhako.",
  "anya-e1007n0:530_0": "43.",
  "anya-e1007n0:531_0": "Byaggho āvudhaviddho hi,",
  "anya-e1007n0:532_0": "Akā duṭṭhāni ninnadaṃ;",
  "anya-e1007n0:533_0": "Tatheva sādhusatthena,",
  "anya-e1007n0:534_0": "Viddho bālo pakuppito.",
  "anya-e1007n0:535_0": "44.",
  "anya-e1007n0:536_0": "Pivanti lohitaṃ ḍaṃsā,",
  "anya-e1007n0:537_0": "Anto tuṇḍena makkhikā;",
  "anya-e1007n0:538_0": "Bahiddhā parivārenti,",
  "anya-e1007n0:539_0": "Jano tena ḍaṃsāyaye.",
  "anya-e1007n0:540_0": "45.",
  "anya-e1007n0:541_0": "Adhanassa khaṇo appo,",
  "anya-e1007n0:542_0": "Saddhammo appakālino;",
  "anya-e1007n0:543_0": "Appako tena yuñjeyyuṃ,",
  "anya-e1007n0:544_0": "Khaṇaṃ bahuṃ labhetave."
}