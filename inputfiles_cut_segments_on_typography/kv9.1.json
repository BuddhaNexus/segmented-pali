{
  "kv9.1:0.1_0": "Kathāvatthu",
  "kv9.1:0.2_0": "Niyāmapaṇṇāsaka",
  "kv9.1:0.3_0": "Navamavagga",
  "kv9.1:0.4_0": "Ānisaṁsadassāvīkathā",
  "kv9.1:1.1_0": "Ānisaṁsadassāvissa saṁyojanānaṁ pahānanti?",
  "kv9.1:1.2_0": "Āmantā.",
  "kv9.1:1.3_0": "Nanu saṅkhāre aniccato manasikaroto saṁyojanā pahīyantīti?",
  "kv9.1:1.4_0": "Āmantā.",
  "kv9.1:1.5_0": "Hañci saṅkhāre aniccato manasikaroto saṁyojanā pahīyanti, no ca vata re vattabbe—",
  "kv9.1:1.6_0": "“ānisaṁsadassāvissa saṁyojanānaṁ pahānan”ti.",
  "kv9.1:2.1_0": "Nanu saṅkhāre dukkhato …pe…",
  "kv9.1:2.2_0": "rogato …",
  "kv9.1:2.3_0": "gaṇḍato …",
  "kv9.1:2.4_0": "sallato …",
  "kv9.1:2.5_0": "aghato …",
  "kv9.1:2.6_0": "ābādhato …",
  "kv9.1:2.7_0": "parato …",
  "kv9.1:2.8_0": "palokato …",
  "kv9.1:2.9_0": "ītito …",
  "kv9.1:2.10_0": "upaddavato …",
  "kv9.1:2.11_0": "bhayato …",
  "kv9.1:2.12_0": "upasaggato …",
  "kv9.1:2.13_0": "calato …",
  "kv9.1:2.14_0": "pabhaṅguto …",
  "kv9.1:2.15_0": "addhuvato …",
  "kv9.1:2.16_0": "atāṇato …",
  "kv9.1:2.17_0": "aleṇato …",
  "kv9.1:2.18_0": "asaraṇato …",
  "kv9.1:2.19_0": "asaraṇībhūtato …",
  "kv9.1:2.20_0": "rittato …",
  "kv9.1:2.21_0": "tucchato …",
  "kv9.1:2.22_0": "suññato …",
  "kv9.1:2.23_0": "anattato …",
  "kv9.1:2.24_0": "ādīnavato …pe…",
  "kv9.1:2.25_0": "vipariṇāmadhammato manasikaroto saṁyojanā pahīyantīti?",
  "kv9.1:2.26_0": "Āmantā.",
  "kv9.1:2.27_0": "Hañci saṅkhāre vipariṇāmadhammato manasikaroto saṁyojanā pahīyanti, no ca vata re vattabbe—",
  "kv9.1:2.28_0": "“ānisaṁsadassāvissa saṁyojanānaṁ pahānan”ti.",
  "kv9.1:3.1_0": "Saṅkhāre ca aniccato manasi karoti nibbāne ca ānisaṁsadassāvī hotīti?",
  "kv9.1:3.2_0": "Na hevaṁ vattabbe …pe…",
  "kv9.1:3.3_0": "saṅkhāre ca aniccato manasi karoti nibbāne ca ānisaṁsadassāvī hotīti?",
  "kv9.1:3.4_0": "Āmantā.",
  "kv9.1:3.5_0": "Dvinnaṁ phassānaṁ …pe…",
  "kv9.1:3.6_0": "dvinnaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv9.1:3.7_0": "Na hevaṁ vattabbe …pe…",
  "kv9.1:3.8_0": "saṅkhāre ca dukkhato …pe…",
  "kv9.1:3.9_0": "vipariṇāmadhammato manasi karoti nibbāne ca ānisaṁsadassāvī hotīti?",
  "kv9.1:3.10_0": "Na hevaṁ vattabbe …pe…",
  "kv9.1:3.11_0": "saṅkhāre ca vipariṇāmadhammato manasi karoti nibbāne ca ānisaṁsadassāvī hotīti?",
  "kv9.1:3.12_0": "Āmantā.",
  "kv9.1:3.13_0": "Dvinnaṁ phassānaṁ …pe…",
  "kv9.1:3.14_0": "dvinnaṁ cittānaṁ samodhānaṁ hotīti?",
  "kv9.1:3.15_0": "Na hevaṁ vattabbe …pe….",
  "kv9.1:4.1_0": "Na vattabbaṁ—",
  "kv9.1:4.2_0": "“ānisaṁsadassāvissa saṁyojanānaṁ pahānan”ti?",
  "kv9.1:4.3_0": "Āmantā.",
  "kv9.1:4.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv9.1:4.5_0": "“idha,",
  "kv9.1:4.5_1": "bhikkhave,",
  "kv9.1:4.5_2": "bhikkhu nibbāne sukhānupassī viharati sukhasaññī sukhapaṭisaṁvedī,",
  "kv9.1:4.5_3": "satataṁ samitaṁ abbokiṇṇaṁ cetasā adhimuccamāno paññāya pariyogāhamāno”ti.",
  "kv9.1:4.6_0": "Attheva suttantoti?",
  "kv9.1:4.7_0": "Āmantā.",
  "kv9.1:4.8_0": "Tena hi ānisaṁsadassāvissa saṁyojanānaṁ pahānanti.",
  "kv9.1:4.9_0": "Ānisaṁsadassāvīkathā niṭṭhitā."
}