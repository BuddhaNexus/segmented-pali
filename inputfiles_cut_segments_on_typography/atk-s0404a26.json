{
  "atk-s0404a26:0_0": "(12) 2. Paccorohaṇivaggo",
  "atk-s0404a26:1_0": "1-2. Adhammasuttadvayavaṇṇanā",
  "atk-s0404a26:2_0": "113-114. Dutiyassa paṭhame pāṭiyekkaṁ pucchā ca vissajjanā ca katā. Dutiye ekatova.",
  "atk-s0404a26:3_0": "3. Tatiyaadhammasuttavaṇṇanā",
  "atk-s0404a26:4_0": "115.",
  "atk-s0404a26:4_1": "Tatiye uddesaṁ uddisitvāti mātikaṁ nikkhipitvā.",
  "atk-s0404a26:4_2": "Satthuceva saṁvaṇṇitoti pañcasu ṭhānesu etadagge ṭhapentena satthārā saṁvaṇṇito.",
  "atk-s0404a26:4_3": "Sambhāvitoti guṇasambhāvanāya sambhāvito.",
  "atk-s0404a26:4_4": "Pahotīti sakkoti.",
  "atk-s0404a26:4_5": "Atisitvāti atikkamitvā.",
  "atk-s0404a26:4_6": "Jānaṁ jānātīti jānitabbakaṁ jānāti.",
  "atk-s0404a26:4_7": "Passaṁ passatīti passitabbakaṁ passati.",
  "atk-s0404a26:4_8": "Cakkhubhūtoti cakkhu viya bhūto jāto nibbatto.",
  "atk-s0404a26:4_9": "Ñāṇabhūtoti ñāṇasabhāvo.",
  "atk-s0404a26:4_10": "Dhammabhūtoti dhammasabhāvo.",
  "atk-s0404a26:4_11": "Brahmabhūtoti seṭṭhasabhāvo.",
  "atk-s0404a26:4_12": "Vattāti vattuṁ samattho.",
  "atk-s0404a26:4_13": "Pavattāti pavattetuṁ samattho.",
  "atk-s0404a26:4_14": "Atthassa ninnetāti atthaṁ nīharitvā dassetā.",
  "atk-s0404a26:4_15": "Yathā no bhagavāti yathā amhākaṁ bhagavā byākareyya.",
  "atk-s0404a26:5_0": "4. Ajitasuttavaṇṇanā",
  "atk-s0404a26:6_0": "116.",
  "atk-s0404a26:6_1": "Catutthe ajitoti evaṁnāmako.",
  "atk-s0404a26:6_2": "Cittaṭṭhānasatānīti cittuppādasatāni.",
  "atk-s0404a26:6_3": "Yehīti yehi cittaṭṭhānasatehi anuyuñjiyamānā.",
  "atk-s0404a26:6_4": "Upāraddhāva jānanti upāraddhasmāti viraddhā niggahitā evaṁ jānanti “viraddhā mayaṁ,",
  "atk-s0404a26:6_5": "niggahitā mayaṁ,",
  "atk-s0404a26:6_6": "āropito no doso”ti.",
  "atk-s0404a26:6_7": "Paṇḍitavatthūnīti paṇḍitabhāvatthāya kāraṇāni.",
  "atk-s0404a26:7_0": "5-6. Saṅgāravasuttādivaṇṇanā",
  "atk-s0404a26:8_0": "117- 118.",
  "atk-s0404a26:8_1": "Pañcame orimaṁ tīranti lokiyaṁ orimatīraṁ.",
  "atk-s0404a26:8_2": "Pārimaṁ tīranti lokuttaraṁ pārimatīraṁ.",
  "atk-s0404a26:8_3": "Pāragāminoti nibbānagāmino.",
  "atk-s0404a26:8_4": "Tīramevānudhāvatīti sakkāyadiṭṭhitīraṁyeva anudhāvati.",
  "atk-s0404a26:8_5": "Dhammedhammānuvattinoti sammā akkhāte navavidhe lokuttaradhamme anudhammavattino,",
  "atk-s0404a26:8_6": "tassa dhammassānucchavikāya sahasīlāya pubbabhāgapaṭipattiyā pavattamānā.",
  "atk-s0404a26:8_7": "Maccudheyyaṁsuduttaranti maccuno ṭhānabhūtaṁ tebhūmakavaṭṭaṁ suduttaraṁ taritvā.",
  "atk-s0404a26:8_8": "Pāramessantīti nibbānaṁ pāpuṇissanti.",
  "atk-s0404a26:9_0": "Okā anokamāgammāti vaṭṭato vivaṭṭaṁ āgamma.",
  "atk-s0404a26:9_1": "Viveke yattha dūramanti yasmiṁ kāyacittaupadhiviveke durabhiramaṁ,",
  "atk-s0404a26:9_2": "tatrābhiratimiccheyya.",
  "atk-s0404a26:9_3": "Hitvā kāmeti duvidhepi kāme pahāya.",
  "atk-s0404a26:9_4": "Akiñcanoti nippalibodho.",
  "atk-s0404a26:9_5": "Ādānapaṭinissageti gahaṇapaṭinissaggasaṅkhāte nibbāne.",
  "atk-s0404a26:9_6": "Anupādāya ye ratāti catūhi upādānehi kiñcipi anupādiyitvā ye abhiratā.",
  "atk-s0404a26:9_7": "Parinibbutāti te apaccayaparinibbānena parinibbutā nāmāti veditabbā.",
  "atk-s0404a26:9_8": "Chaṭṭhaṁ bhikkhūnaṁ desitaṁ.",
  "atk-s0404a26:10_0": "7-8. Paccorohaṇīsuttadvayavaṇṇanā",
  "atk-s0404a26:11_0": "119- 120.",
  "atk-s0404a26:11_1": "Sattame paccorohaṇīti pāpassa paccorohaṇaṁ.",
  "atk-s0404a26:11_2": "Pattharitvāti santharitvā.",
  "atk-s0404a26:11_3": "Antarā ca velaṁ antarā ca agyāgāranti vālikārāsissa ca aggiagārassa ca antare.",
  "atk-s0404a26:11_4": "Aṭṭhamaṁ bhikkhusaṅghassa desitaṁ.",
  "atk-s0404a26:11_5": "Sesaṁ sabbattha uttānatthamevāti.",
  "atk-s0404a26:12_0": "Paccorohaṇivaggo dutiyo."
}