{
  "tika-vin09t23:0_0": "Nissaggiyakaṇḍaṁ",
  "tika-vin09t23:1_0": "1. Pattavaggo",
  "tika-vin09t23:2_0": "1. Pattasannicayasikkhāpadavaṇṇanā",
  "tika-vin09t23:3_0": "Paṭhamaṁ uttānatthameva.",
  "tika-vin09t23:4_0": "Pattasannicayasikkhāpadavaṇṇanā niṭṭhitā.",
  "tika-vin09t23:5_0": "2. Akālacīvarasikkhāpadavaṇṇanā",
  "tika-vin09t23:6_0": "Yaṁ attanā laddhaṁ,",
  "tika-vin09t23:6_1": "taṁ nissaggiyaṁ hotīti yaṁ cīvaraṁ bhājāpitāya attanā laddhaṁ,",
  "tika-vin09t23:6_2": "tameva nissaggiyaṁ hoti,",
  "tika-vin09t23:6_3": "taṁ vinayakammaṁ katvāpi attanā na labbhati.",
  "tika-vin09t23:6_4": "Tenāha “nissaṭṭha”ntiādi.",
  "tika-vin09t23:6_5": "Tattha nissaṭṭhaṁ paṭilabhitvāpīti “ayyāya,",
  "tika-vin09t23:6_6": "dammī”ti evaṁ nissaṭṭhaṁ paṭilabhitvāpi.",
  "tika-vin09t23:6_7": "Yathādāneyeva upanetabbanti yathā dāyakehi dinnaṁ,",
  "tika-vin09t23:6_8": "tatheva upanetabbaṁ,",
  "tika-vin09t23:6_9": "akālacīvarapakkheyeva ṭhapetabbanti vuttaṁ hoti.",
  "tika-vin09t23:7_0": "Akālacīvarasikkhāpadavaṇṇanā niṭṭhitā.",
  "tika-vin09t23:8_0": "3. Cīvaraparivattanasikkhāpadavaṇṇanā",
  "tika-vin09t23:9_0": "Metanti me etaṁ.",
  "tika-vin09t23:9_1": "Sakasaññāya gahitattā pācittiyañceva dukkaṭañca vuttaṁ,",
  "tika-vin09t23:9_2": "itarathā bhaṇḍagghena kāretabbaṁ.",
  "tika-vin09t23:10_0": "Cīvaraparivattanasikkhāpadavaṇṇanā niṭṭhitā.",
  "tika-vin09t23:11_0": "4. Aññaviññāpanasikkhāpadavaṇṇanā",
  "tika-vin09t23:12_0": "Paṭilābhenāti paṭhamaviññattito ūnatarassāpi aññassa paṭilābhena.",
  "tika-vin09t23:13_0": "Tasmiṁ appahonte puna taññevāti yaṁ paṭhamaṁ viññattaṁ,",
  "tika-vin09t23:13_1": "thokattā tasmiṁ appahonte puna taññeva viññāpentiyāti attho.",
  "tika-vin09t23:13_2": "Aññenapi atthe satī ti paṭhamaṁ viññattito aññenapi atthe sati.",
  "tika-vin09t23:13_3": "Tena saddhiṁ aññañcāti yaṁ paṭhamaṁ viññattaṁ,",
  "tika-vin09t23:13_4": "tena saddhiṁ aññañca.",
  "tika-vin09t23:13_5": "Kiṁ vuttaṁ hoti?",
  "tika-vin09t23:13_6": "Sace paṭhamaṁ sappi viññattaṁ,",
  "tika-vin09t23:13_7": "“yamakaṁ pacitabba”nti ca vejjena vuttattā telenāpi attho hoti.",
  "tika-vin09t23:13_8": "Tato “telenāpi me attho”ti evamādinā aññañca viññāpentiyāti vuttaṁ hoti.",
  "tika-vin09t23:13_9": "Ānisaṁsaṁ dassetvāti “sace kahāpaṇassa sappi ābhataṁ hoti,",
  "tika-vin09t23:13_10": "iminā mūlena dviguṇaṁ telaṁ labbhati,",
  "tika-vin09t23:13_11": "telenāpi ca idaṁ kiccaṁ nippajjati,",
  "tika-vin09t23:13_12": "tasmā telaṁ āharā”ti evaṁ ānisaṁsaṁ dassetvā.",
  "tika-vin09t23:14_0": "Aññaviññāpanasikkhāpadavaṇṇanā niṭṭhitā.",
  "tika-vin09t23:15_0": "5. Aññacetāpanasikkhāpadavaṇṇanā",
  "tika-vin09t23:16_0": "Tato aññaṁ cetāpeyyāti paṭhamaṁ cetāpitato yaṁ kiñci aññaṁ cetāpeyya.",
  "tika-vin09t23:17_0": "Aññacetāpanasikkhāpadavaṇṇanā niṭṭhitā.",
  "tika-vin09t23:18_0": "6. Paṭhamasaṅghikacetāpanasikkhāpadavaṇṇanā",
  "tika-vin09t23:19_0": "Aññassatthāya dinnenāti yaṁ cetāpeti,",
  "tika-vin09t23:19_1": "tato aññassatthāya dinnena.",
  "tika-vin09t23:19_2": "Aññuddisikenāti purimassevatthadīpanaṁ.",
  "tika-vin09t23:20_0": "Sesakaṁ upanentiyāti yadatthāya dinno,",
  "tika-vin09t23:20_1": "taṁ cetāpetvā avasesaṁ aññassatthāya upanentiyā.",
  "tika-vin09t23:20_2": "Sāmike apaloketvāti dāyake āpucchitvā.",
  "tika-vin09t23:20_3": "Evarūpāsū āpadāsu upanentīnanti evarūpesu upaddavesu aññassa yassa kassaci atthāya upanentīnaṁ.",
  "tika-vin09t23:21_0": "Paṭhamasaṅghikacetāpanasikkhāpadavaṇṇanā niṭṭhitā.",
  "tika-vin09t23:22_0": "7. Dutiyasaṅghikacetāpanasikkhāpadavaṇṇanā",
  "tika-vin09t23:23_0": "Na kevalaṁ teneva parikkhārenāti āha “sayaṁ yācitakenāpī”ti.",
  "tika-vin09t23:23_1": "Vuttañhetaṁ pāḷiyaṁ “bhikkhuniyo tena ca parikkhārena sayampi yācitvā bhesajjaṁ cetāpetvā paribhuñjiṁsū”ti.",
  "tika-vin09t23:23_2": "Mātikāyaṁ pana gamyamānattā pi-",
  "tika-vin09t23:23_3": "saddo nappayutto.",
  "tika-vin09t23:23_4": "Gamyamānatthassa hi saddassa payogaṁ pati kāmacāroti yutti.",
  "tika-vin09t23:24_0": "Dutiyasaṅghikacetāpanasikkhāpadavaṇṇanā niṭṭhitā.",
  "tika-vin09t23:25_0": "8-9-10. Paṭhamagaṇikacetāpanādisikkhāpadavaṇṇanā",
  "tika-vin09t23:26_0": "Aṭṭhamanavamadasamāni uttānatthāneva.",
  "tika-vin09t23:27_0": "Paṭhamagaṇikacetāpanādisikkhāpadavaṇṇanā niṭṭhitā.",
  "tika-vin09t23:28_0": "Pattavaggo paṭhamo.",
  "tika-vin09t23:29_0": "2. Cīvaravaggo",
  "tika-vin09t23:30_0": "11. Garupāvuraṇasikkhāpadavaṇṇanā",
  "tika-vin09t23:31_0": "Yasmā pavāritaṭṭhāne viññatti nāma na paṭisedhetabbā,",
  "tika-vin09t23:31_1": "tasmā bhagavā dhammanimantanavasena pavāritaṭṭhāne “vadethāyye,",
  "tika-vin09t23:31_2": "yenattho”ti vuttāya catukkaṁsaparamaṁ viññāpetabbanti paricchedaṁ dassetīti veditabbaṁ.",
  "tika-vin09t23:31_3": "Teneva “cetāpetabbanti ṭhapetvā sahadhammike ca ñātakappavārite ca aññena kismiñcideva guṇe parituṭṭhena ‘vadethāyye,",
  "tika-vin09t23:31_4": "yenattho’ti vuttāya viññāpetabba”nti vuttaṁ.",
  "tika-vin09t23:31_5": "Rājānanti pasenadikosalarājānaṁ.",
  "tika-vin09t23:32_0": "Garupāvuraṇasikkhāpadavaṇṇanā niṭṭhitā.",
  "tika-vin09t23:33_0": "12. Lahupāvuraṇasikkhāpadavaṇṇanā",
  "tika-vin09t23:34_0": "Dutiyaṁ uttānatthameva.",
  "tika-vin09t23:35_0": "Lahupāvuraṇasikkhāpadavaṇṇanā niṭṭhitā.",
  "tika-vin09t23:36_0": "Cīvaravaggo dutiyo.",
  "tika-vin09t23:37_0": "Iti kaṅkhāvitaraṇiyā pātimokkhavaṇṇanāya",
  "tika-vin09t23:38_0": "Vinayatthamañjūsāyaṁ līnatthappakāsaniyaṁ",
  "tika-vin09t23:39_0": "Bhikkhunipātimokkhe nissaggiyavaṇṇanā niṭṭhitā."
}