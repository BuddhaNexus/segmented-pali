{
  "anya-e1008n:0_0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1008n:1_0": "Cāṇakyanītipāḷi",
  "anya-e1008n:2_0": "1.",
  "anya-e1008n:3_0": "Nānāsattho ddhataṃ vakkhe,",
  "anya-e1008n:4_0": "Rāja nīti samuccayaṃ;",
  "anya-e1008n:5_0": "Sabba bīja midaṃ satthaṃ,",
  "anya-e1008n:6_0": "Cāṇakyaṃ sārasaṅgahaṃ.",
  "anya-e1008n:7_0": "2.",
  "anya-e1008n:8_0": "Mūlasuttaṃ pavakkhāmi,",
  "anya-e1008n:9_0": "Cāṇakyena yathoditaṃ;",
  "anya-e1008n:10_0": "Yassa viññāna mattena,",
  "anya-e1008n:11_0": "Mūḷho bhavati paṇḍito.",
  "anya-e1008n:12_0": "3.",
  "anya-e1008n:13_0": "Viduttaṃ narapaccañca,",
  "anya-e1008n:14_0": "Nevatulyaṃ kudācanaṃ;",
  "anya-e1008n:15_0": "Sadese pujjate rājā,",
  "anya-e1008n:16_0": "Vidū sabbattha pujjate.",
  "anya-e1008n:17_0": "4.",
  "anya-e1008n:18_0": "Paṇḍite ca guṇā sabbe,",
  "anya-e1008n:19_0": "Mūḷhe dosā hi kevalaṃ;",
  "anya-e1008n:20_0": "Tasmā mūḷhasahassesu,",
  "anya-e1008n:21_0": "Pañño eko visisyate.",
  "anya-e1008n:22_0": "5.",
  "anya-e1008n:23_0": "Mātariva paradāresu,",
  "anya-e1008n:24_0": "Paradabbesu leṭṭuva;",
  "anya-e1008n:25_0": "Attaniva sabbabhūtesu,",
  "anya-e1008n:26_0": "Yo passati sapaṇḍito.",
  "anya-e1008n:27_0": "6.",
  "anya-e1008n:28_0": "Kiṃkulena visālena,",
  "anya-e1008n:29_0": "Guṇahīno tu yonaro;",
  "anya-e1008n:30_0": "Akulinopi satthañño,",
  "anya-e1008n:31_0": "Devatāhipi pujjate.",
  "anya-e1008n:32_0": "7.",
  "anya-e1008n:33_0": "Rūpayobbanasampannā,",
  "anya-e1008n:34_0": "Visālakulasambhavā;",
  "anya-e1008n:35_0": "Vijjāhīnā nasobhante,",
  "anya-e1008n:36_0": "Niggandhā iva kiṃsukā.",
  "anya-e1008n:37_0": "8.",
  "anya-e1008n:38_0": "Tārānaṃ bhūsaṇaṃ cando,",
  "anya-e1008n:39_0": "Nārīnaṃ bhūsaṇaṃ pati;",
  "anya-e1008n:40_0": "Puthabyā bhūsaṇaṃ rājā,",
  "anya-e1008n:41_0": "Vijjā sabbassa bhūsaṇaṃ.",
  "anya-e1008n:42_0": "9.",
  "anya-e1008n:43_0": "Mātā sattu pitā verī,",
  "anya-e1008n:44_0": "Yena bālo napāṭṭhito;",
  "anya-e1008n:45_0": "Na sobhate sabhāmajjhe,",
  "anya-e1008n:46_0": "Haṃsamajjhe bakoyathā.",
  "anya-e1008n:47_0": "10.",
  "anya-e1008n:48_0": "Varameko guṇī putto,",
  "anya-e1008n:49_0": "Na ca mūḷhasatehipi;",
  "anya-e1008n:50_0": "Eko cando tamo hanti,",
  "anya-e1008n:51_0": "Na ca tāragaṇehipi.",
  "anya-e1008n:52_0": "11.",
  "anya-e1008n:53_0": "Lālaye pañcavassāni,",
  "anya-e1008n:54_0": "Dasavassāni tālaye;",
  "anya-e1008n:55_0": "Patte tu soḷase vasse,",
  "anya-e1008n:56_0": "Puttaṃ mittaṃva ācare.",
  "anya-e1008n:57_0": "12.",
  "anya-e1008n:58_0": "Lālane bahavo dosā,",
  "anya-e1008n:59_0": "Tālane bahavo guṇā;",
  "anya-e1008n:60_0": "Tasmā puttañca sissañca,",
  "anya-e1008n:61_0": "Tālaye na tu lālaye.",
  "anya-e1008n:62_0": "13.",
  "anya-e1008n:63_0": "Ekenāpi suvakkhena,",
  "anya-e1008n:64_0": "Pupphitena sugandhinā;",
  "anya-e1008n:65_0": "Vāsitassa vanaṃ sabbaṃ,",
  "anya-e1008n:66_0": "Suputtena kulaṃyathā.",
  "anya-e1008n:67_0": "14.",
  "anya-e1008n:68_0": "Ekassāpi kuvakkhassa,",
  "anya-e1008n:69_0": "Koṭaraṭṭhena agginā;",
  "anya-e1008n:70_0": "Dayhate hi vanaṃ sabbaṃ,",
  "anya-e1008n:71_0": "Kuputtena kulaṃ yathā.",
  "anya-e1008n:72_0": "15.",
  "anya-e1008n:73_0": "Dūrato sobhate mūḷho,",
  "anya-e1008n:74_0": "Lambamāna paṭāvuto;",
  "anya-e1008n:75_0": "Tāvañca sobhate mūḷho,",
  "anya-e1008n:76_0": "Yāva kiñci nabhāsate.",
  "anya-e1008n:77_0": "16.",
  "anya-e1008n:78_0": "Visato amataṃ gāyhaṃ,",
  "anya-e1008n:79_0": "Amejjhāapi kañcanaṃ;",
  "anya-e1008n:80_0": "Nīcato uttamā vijjā,",
  "anya-e1008n:81_0": "Thīratnaṃ dukkulāapi.",
  "anya-e1008n:82_0": "17.",
  "anya-e1008n:83_0": "Ussave byasaneceva,",
  "anya-e1008n:84_0": "Dubbhikkhe sattuviggahe;",
  "anya-e1008n:85_0": "Rājadvāre susāneca,",
  "anya-e1008n:86_0": "Yo tiṭṭhati sabandhavo.",
  "anya-e1008n:87_0": "18.",
  "anya-e1008n:88_0": "Parokkhe kiccahantāraṃ,",
  "anya-e1008n:89_0": "Paccakkhe piyavādinaṃ;",
  "anya-e1008n:90_0": "Vajjaye tādisaṃ mittaṃ,",
  "anya-e1008n:91_0": "Visakumbhaṃ payomukhaṃ.",
  "anya-e1008n:92_0": "19.",
  "anya-e1008n:93_0": "Sakiṃ duṭṭhañca mittaṃ yo,",
  "anya-e1008n:94_0": "Puna sandhātu micchati;",
  "anya-e1008n:95_0": "Samaccu mupagaṇhāti,",
  "anya-e1008n:96_0": "Gabbha massatarī yathā.",
  "anya-e1008n:97_0": "20.",
  "anya-e1008n:98_0": "Na vissase avissatthaṃ,",
  "anya-e1008n:99_0": "Mittañcāpi na vissase;",
  "anya-e1008n:100_0": "Kadāci kupitaṃ mittaṃ,",
  "anya-e1008n:101_0": "Sabbadosaṃ pakāsaye.",
  "anya-e1008n:102_0": "21.",
  "anya-e1008n:103_0": "Jāniyā pesane bhacce,",
  "anya-e1008n:104_0": "Bandhave byasanāgame;",
  "anya-e1008n:105_0": "Mittañcā padikāleca,",
  "anya-e1008n:106_0": "Bhariyañca vibhavakkhaye.",
  "anya-e1008n:107_0": "22.",
  "anya-e1008n:108_0": "Upakāraggahitena,",
  "anya-e1008n:109_0": "Sattunāsattumuddhare;",
  "anya-e1008n:110_0": "Pādalaggaṃ karaṭṭhena,",
  "anya-e1008n:111_0": "Kaṇṭakeneva kaṇṭakaṃ.",
  "anya-e1008n:112_0": "23.",
  "anya-e1008n:113_0": "Na mittaṃ koci kassaci,",
  "anya-e1008n:114_0": "Na koci ripu kassaci;",
  "anya-e1008n:115_0": "Kāraṇena hi ñāyati,",
  "anya-e1008n:116_0": "Mittāni ca ripū tathā.",
  "anya-e1008n:117_0": "24.",
  "anya-e1008n:118_0": "Dujjano piyavādī ca,",
  "anya-e1008n:119_0": "Netaṃ vissāsakāraṇaṃ;",
  "anya-e1008n:120_0": "Madhu tiṭṭhati jivhagge,",
  "anya-e1008n:121_0": "Hadaye tu halāhalaṃ.",
  "anya-e1008n:122_0": "25.",
  "anya-e1008n:123_0": "Dujjano parihantabbo,",
  "anya-e1008n:124_0": "Vijjāyā laṅkatopi saṃ;",
  "anya-e1008n:125_0": "Maṇinā bhūsito sappo,",
  "anya-e1008n:126_0": "Kime so nabhayaṃ karo.",
  "anya-e1008n:127_0": "26.",
  "anya-e1008n:128_0": "Sappo kūro khalo kūro,",
  "anya-e1008n:129_0": "Sappā kūrataro khalo;",
  "anya-e1008n:130_0": "Manto sadhivaso sappo,",
  "anya-e1008n:131_0": "Khalo kena nivāyyate.",
  "anya-e1008n:132_0": "27.",
  "anya-e1008n:133_0": "Nakhīnañca nadīnañca,",
  "anya-e1008n:134_0": "Siṅgīnaṃ satthapāṇinaṃ;",
  "anya-e1008n:135_0": "Vissāso nevakātabbo,",
  "anya-e1008n:136_0": "Thīsu rājakulesu ca.",
  "anya-e1008n:137_0": "28.",
  "anya-e1008n:138_0": "Hatthī hatthasahassena,",
  "anya-e1008n:139_0": "Satahatthena vājino;",
  "anya-e1008n:140_0": "Siṅgino dasahatthena,",
  "anya-e1008n:141_0": "Ṭhānacāgena dujjano.",
  "anya-e1008n:142_0": "29.",
  "anya-e1008n:143_0": "Āpadatthaṃ dhanaṃ rakkhe,",
  "anya-e1008n:144_0": "Dāraṃ rakkhe dhanehipi;",
  "anya-e1008n:145_0": "Attānaṃ satataṃ rakkhe,",
  "anya-e1008n:146_0": "Dārehipi dhanehipi.",
  "anya-e1008n:147_0": "30.",
  "anya-e1008n:148_0": "Paradāraṃ paradabbaṃ,",
  "anya-e1008n:149_0": "Parivādaṃ parassa ca;",
  "anya-e1008n:150_0": "Parihāsaṃ guruṭṭhāne,",
  "anya-e1008n:151_0": "Cāpalyañca vivajjaye.",
  "anya-e1008n:152_0": "31.",
  "anya-e1008n:153_0": "Caje ekaṃ kulassatthe,",
  "anya-e1008n:154_0": "Gāmassatthe kulaṃ caje;",
  "anya-e1008n:155_0": "Gāma janapadassatthe,",
  "anya-e1008n:156_0": "Attatthe pathaviṃ caje.",
  "anya-e1008n:157_0": "32.",
  "anya-e1008n:158_0": "Calatyekena pādena,",
  "anya-e1008n:159_0": "Tiṭṭha tyekena buddhimā;",
  "anya-e1008n:160_0": "Nāsamikkhya paraṃ ṭhānaṃ,",
  "anya-e1008n:161_0": "Pubbamāyatanaṃ caje.",
  "anya-e1008n:162_0": "33.",
  "anya-e1008n:163_0": "Luddha matthena gaṇheyya,",
  "anya-e1008n:164_0": "Thaddha mañjalī kammunā;",
  "anya-e1008n:165_0": "Mūḷhaṃ chando nuvattena,",
  "anya-e1008n:166_0": "Tathā tathena paṇḍitaṃ.",
  "anya-e1008n:167_0": "34.",
  "anya-e1008n:168_0": "Atthanāsaṃ manotāpaṃ,",
  "anya-e1008n:169_0": "Gehe duccaritāni ca;",
  "anya-e1008n:170_0": "Vañcanañca pamāṇañca,",
  "anya-e1008n:171_0": "Matimā na pakāsaye.",
  "anya-e1008n:172_0": "35.",
  "anya-e1008n:173_0": "Dhanadhaññappayogesu,",
  "anya-e1008n:174_0": "Tathā vijjāgamesu ca;",
  "anya-e1008n:175_0": "Āhāre byavahāre ca,",
  "anya-e1008n:176_0": "Cattalajjo sadā bhave.",
  "anya-e1008n:177_0": "36.",
  "anya-e1008n:178_0": "Dhanino sotthiyo rājā,",
  "anya-e1008n:179_0": "Nadī vejjo tu pañcamo;",
  "anya-e1008n:180_0": "Pañca yatra navijjante,",
  "anya-e1008n:181_0": "Tatra vāsaṃ nakāraye.",
  "anya-e1008n:182_0": "37.",
  "anya-e1008n:183_0": "Yasmiṃdese na sammānaṃ,",
  "anya-e1008n:184_0": "Na pīti naca bandhavā;",
  "anya-e1008n:185_0": "Na ca vijjāgamo koci,",
  "anya-e1008n:186_0": "Taṃdesaṃ parivajjaye.",
  "anya-e1008n:187_0": "38.",
  "anya-e1008n:188_0": "Manasā cintitaṃ kammaṃ,",
  "anya-e1008n:189_0": "Vacasā napakāsaye;",
  "anya-e1008n:190_0": "Aññalakkhita kāriyassa,",
  "anya-e1008n:191_0": "Yato siddhi najāyate.",
  "anya-e1008n:192_0": "39.",
  "anya-e1008n:193_0": "Kudesañca kuvuttiñca,",
  "anya-e1008n:194_0": "Kubhariyaṃ kunadiṃ tathā;",
  "anya-e1008n:195_0": "Kudabbañca kubhojjañca,",
  "anya-e1008n:196_0": "Vajjaye tu vicakkhaṇo.",
  "anya-e1008n:197_0": "40.",
  "anya-e1008n:198_0": "Iṇasesoggi seso ca,",
  "anya-e1008n:199_0": "Byādhiseso tatheva ca;",
  "anya-e1008n:200_0": "Puna ca vaḍḍhate yasmā,",
  "anya-e1008n:201_0": "Tasmā sesaṃ nakāraye.",
  "anya-e1008n:202_0": "41.",
  "anya-e1008n:203_0": "Cintā jaro manussānaṃ,",
  "anya-e1008n:204_0": "Vatthānaṃ ātapo jaro;",
  "anya-e1008n:205_0": "Asobhagyaṃ jaro thīnaṃ,",
  "anya-e1008n:206_0": "Assānaṃ methunaṃ jaro.",
  "anya-e1008n:207_0": "42.",
  "anya-e1008n:208_0": "Atthi putto vase yassa,",
  "anya-e1008n:209_0": "Bhacco bhariyā tatheva ca;",
  "anya-e1008n:210_0": "Abhāve pyatisantoso,",
  "anya-e1008n:211_0": "Saggaṭṭho so mahītale.",
  "anya-e1008n:212_0": "43.",
  "anya-e1008n:213_0": "Duṭṭhā bhariyā saṭhaṃ mittaṃ,",
  "anya-e1008n:214_0": "Bhacco cuttaradāyako;",
  "anya-e1008n:215_0": "Sa sappeca gahe vāso,",
  "anya-e1008n:216_0": "Maccureva nasaṃsayo.",
  "anya-e1008n:217_0": "44.",
  "anya-e1008n:218_0": "Mātā yassa gehe natthi,",
  "anya-e1008n:219_0": "Bhariyācā piyavādinī;",
  "anya-e1008n:220_0": "Araññaṃ tena gantabbaṃ,",
  "anya-e1008n:221_0": "Yathā raññaṃ tathāgahaṃ.",
  "anya-e1008n:222_0": "45.",
  "anya-e1008n:223_0": "Iṇakattā pitā sattu,",
  "anya-e1008n:224_0": "Mātā ca byabhicārinī;",
  "anya-e1008n:225_0": "Bhariyā rūpavatī sattu,",
  "anya-e1008n:226_0": "Putto sattu apaṇḍito.",
  "anya-e1008n:227_0": "46.",
  "anya-e1008n:228_0": "Kokilānaṃ saro rūpaṃ,",
  "anya-e1008n:229_0": "Nārī rūpaṃ patibbatā;",
  "anya-e1008n:230_0": "Vijjā rūpaṃ kurūpānaṃ,",
  "anya-e1008n:231_0": "Khamā rūpaṃ tapassinaṃ.",
  "anya-e1008n:232_0": "47.",
  "anya-e1008n:233_0": "Avijjaṃ jīvanaṃ suññaṃ,",
  "anya-e1008n:234_0": "Disā suññā abandhavā;",
  "anya-e1008n:235_0": "Puttahīnaṃ gahaṃ suññaṃ,",
  "anya-e1008n:236_0": "Sabbasuññā daliddatā.",
  "anya-e1008n:237_0": "48.",
  "anya-e1008n:238_0": "Adātā vaṃsadosena,",
  "anya-e1008n:239_0": "Kammadosā daliddatā;",
  "anya-e1008n:240_0": "Ummādā mātudosena,",
  "anya-e1008n:241_0": "Pitudosena mūḷhatā.",
  "anya-e1008n:242_0": "49.",
  "anya-e1008n:243_0": "Guru aggi dvijādīnaṃ,",
  "anya-e1008n:244_0": "Vaṇṇānaṃ brāhmaṇo guru;",
  "anya-e1008n:245_0": "Pati reko gurutthīnaṃ,",
  "anya-e1008n:246_0": "Sabbassābhyāgato guru.",
  "anya-e1008n:247_0": "50.",
  "anya-e1008n:248_0": "Atidabbe hatā laṅkā,",
  "anya-e1008n:249_0": "Atimāne ca koravā;",
  "anya-e1008n:250_0": "Atidāne balībaddho,",
  "anya-e1008n:251_0": "Sabbamaccanta gahitaṃ.",
  "anya-e1008n:252_0": "51.",
  "anya-e1008n:253_0": "Vatthahīno tvalaṅkāro,",
  "anya-e1008n:254_0": "Ghatahīnañca bhojanaṃ;",
  "anya-e1008n:255_0": "Thanahīnā ca yānārī,",
  "anya-e1008n:256_0": "Vijjāhīnañca jīvanaṃ.",
  "anya-e1008n:257_0": "52.",
  "anya-e1008n:258_0": "Bhojjaṃ bhojanasatti ca,",
  "anya-e1008n:259_0": "Ratisatti varā thiyo;",
  "anya-e1008n:260_0": "Vibhavo dānasatti ca,",
  "anya-e1008n:261_0": "Nāppassa tapaso phalaṃ.",
  "anya-e1008n:262_0": "53.",
  "anya-e1008n:263_0": "Puttappayojanā dārā,",
  "anya-e1008n:264_0": "Putto piṇḍappayojano;",
  "anya-e1008n:265_0": "Hitappayojanaṃ mittaṃ,",
  "anya-e1008n:266_0": "Dhanaṃ sabbappayojanaṃ.",
  "anya-e1008n:267_0": "54.",
  "anya-e1008n:268_0": "Dullabhaṃ pākatikaṃ vākyaṃ,",
  "anya-e1008n:269_0": "Dullabho khemakaro suto;",
  "anya-e1008n:270_0": "Dullabhā sadisī jāyā,",
  "anya-e1008n:271_0": "Dullabho sajano piyo.",
  "anya-e1008n:272_0": "55.",
  "anya-e1008n:273_0": "Selesele namāṇikkaṃ,",
  "anya-e1008n:274_0": "Muttikaṃ na gajegaje;",
  "anya-e1008n:275_0": "Sādhavo na hi sabbatra,",
  "anya-e1008n:276_0": "Candanaṃ na vanevane.",
  "anya-e1008n:277_0": "56.",
  "anya-e1008n:278_0": "Asoco niddhano pañño,",
  "anya-e1008n:279_0": "Asoco paṇḍitabandhavo;",
  "anya-e1008n:280_0": "Asocā vidhavā nārī,",
  "anya-e1008n:281_0": "Puttanatta patiṭṭhitā.",
  "anya-e1008n:282_0": "57.",
  "anya-e1008n:283_0": "Avijjo puriso soco,",
  "anya-e1008n:284_0": "Socaṃ methuna mappajaṃ;",
  "anya-e1008n:285_0": "Nirāhārā pajā socā,",
  "anya-e1008n:286_0": "Socaṃ rajja marājakaṃ.",
  "anya-e1008n:287_0": "58.",
  "anya-e1008n:288_0": "Kulehi saha sampakkaṃ,",
  "anya-e1008n:289_0": "Paṇḍitehi ca mittataṃ;",
  "anya-e1008n:290_0": "Ñātībhi ca samaṃ melaṃ,",
  "anya-e1008n:291_0": "Kubbāno navinassati.",
  "anya-e1008n:292_0": "59.",
  "anya-e1008n:293_0": "Kaṭṭhā vutti parādhinā,",
  "anya-e1008n:294_0": "Kaṭṭho vāso nirāsayo;",
  "anya-e1008n:295_0": "Niddhano byavasāyo ca,",
  "anya-e1008n:296_0": "Sabbakaṭṭhā daliddatā.",
  "anya-e1008n:297_0": "60.",
  "anya-e1008n:298_0": "Takkarassa kuto dhammo,",
  "anya-e1008n:299_0": "Dujjanassa kuto khamā;",
  "anya-e1008n:300_0": "Vesiyā ca kuto sneho,",
  "anya-e1008n:301_0": "Kuto saccañca kāminaṃ.",
  "anya-e1008n:302_0": "61.",
  "anya-e1008n:303_0": "Pesitassa kuto mānaṃ,",
  "anya-e1008n:304_0": "Kopanassa kuto sukhaṃ;",
  "anya-e1008n:305_0": "Thīnaṃ kuto satittañca,",
  "anya-e1008n:306_0": "Kuto mettī khalassa ca.",
  "anya-e1008n:307_0": "62.",
  "anya-e1008n:308_0": "Dubbalassa balaṃ rājā,",
  "anya-e1008n:309_0": "Bālānaṃ rodanaṃ balaṃ;",
  "anya-e1008n:310_0": "Balaṃmūḷhassa monittaṃ,",
  "anya-e1008n:311_0": "Corānaṃ atathaṃ balaṃ.",
  "anya-e1008n:312_0": "63.",
  "anya-e1008n:313_0": "Yo dhuvāni pariccajja,",
  "anya-e1008n:314_0": "Adhuvaṃ parisevati;",
  "anya-e1008n:315_0": "Dhuvāni tassa nassanti,",
  "anya-e1008n:316_0": "Adhuvaṃ naṭṭhameva ca.",
  "anya-e1008n:317_0": "64.",
  "anya-e1008n:318_0": "Sukkaṃ maṃsaṃ thiyo vuddhā,",
  "anya-e1008n:319_0": "Bālakka taruṇaṃ dadhi;",
  "anya-e1008n:320_0": "Pabhāte methunaṃ niddā,",
  "anya-e1008n:321_0": "Sajju pāṇaharāni cha;",
  "anya-e1008n:322_0": "65.",
  "anya-e1008n:323_0": "Sajju maṃsaṃ navannañca,",
  "anya-e1008n:324_0": "Bālā thī khīrabhojanaṃ;",
  "anya-e1008n:325_0": "Ghatamuṇhodakañceva,",
  "anya-e1008n:326_0": "Sajju pāṇakarāni cha.",
  "anya-e1008n:327_0": "66.",
  "anya-e1008n:328_0": "Sīhādekaṃ bakādekaṃ,",
  "anya-e1008n:329_0": "Cha sunā tīṇi gadrabhā;",
  "anya-e1008n:330_0": "Vāyasā catu sikkhetha,",
  "anya-e1008n:331_0": "Cattāri kukkuṭādapi.",
  "anya-e1008n:332_0": "67.",
  "anya-e1008n:333_0": "Pabhūtamappakiccaṃ vā,",
  "anya-e1008n:334_0": "Yonaro kattumicchati;",
  "anya-e1008n:335_0": "Saṃyatanena kattabbaṃ,",
  "anya-e1008n:336_0": "Sīhādekaṃ pakittitaṃ.",
  "anya-e1008n:337_0": "68.",
  "anya-e1008n:338_0": "Sabbindriyāni saṃyama,",
  "anya-e1008n:339_0": "Bakova patito jano;",
  "anya-e1008n:340_0": "Kāladesopapannāni,",
  "anya-e1008n:341_0": "Sabbakiccāni sādhaye.",
  "anya-e1008n:342_0": "69.",
  "anya-e1008n:343_0": "Bahvāsī sāppasantuṭṭho,",
  "anya-e1008n:344_0": "Suniddo sīghacetano;",
  "anya-e1008n:345_0": "Pabhubhatto ca sūro ca,",
  "anya-e1008n:346_0": "Ñātabbā cha sunā guṇā.",
  "anya-e1008n:347_0": "70.",
  "anya-e1008n:348_0": "Avissāmaṃ vahe bhāraṃ,",
  "anya-e1008n:349_0": "Sītuṇhañca navindati;",
  "anya-e1008n:350_0": "Sa santoso tathā niccaṃ,",
  "anya-e1008n:351_0": "Tīṇi sikkhetha gadrabhā.",
  "anya-e1008n:352_0": "71.",
  "anya-e1008n:353_0": "Guḷhamethunadhammañca,",
  "anya-e1008n:354_0": "Kālekāle ca saṅgahaṃ;",
  "anya-e1008n:355_0": "Appamādamanālasyaṃ,",
  "anya-e1008n:356_0": "Catu sikkhetha vāyasā.",
  "anya-e1008n:357_0": "72.",
  "anya-e1008n:358_0": "Yuddhañca pātaruṭṭhānaṃ,",
  "anya-e1008n:359_0": "Bhojanaṃ saha bandhuhi;",
  "anya-e1008n:360_0": "Thiyaṃ āpadaggataṃ rakkhe,",
  "anya-e1008n:361_0": "Catu sikkhetha kukkuṭā.",
  "anya-e1008n:362_0": "73.",
  "anya-e1008n:363_0": "Kotibhāro samatthānaṃ,",
  "anya-e1008n:364_0": "Kiṃdūraṃ byavasāyinaṃ;",
  "anya-e1008n:365_0": "Ko videso savijjānaṃ,",
  "anya-e1008n:366_0": "Ko paro piyavādinaṃ.",
  "anya-e1008n:367_0": "74.",
  "anya-e1008n:368_0": "Bhayassa kathito pantho,",
  "anya-e1008n:369_0": "Indriyānamasaṃyamo;",
  "anya-e1008n:370_0": "Tajjayo sampadāmaggo,",
  "anya-e1008n:371_0": "Yeniṭṭhaṃ tena gamyate.",
  "anya-e1008n:372_0": "75.",
  "anya-e1008n:373_0": "Na ca vijjāsamo bandhu,",
  "anya-e1008n:374_0": "Na ca byādhisamo ripu;",
  "anya-e1008n:375_0": "Nacāpaccasamo sneho,",
  "anya-e1008n:376_0": "Na ca devā paraṃ balaṃ.",
  "anya-e1008n:377_0": "76.",
  "anya-e1008n:378_0": "Samuddāvaraṇā bhūmi,",
  "anya-e1008n:379_0": "Pākārāvaraṇaṃ gahaṃ;",
  "anya-e1008n:380_0": "Narindāvaraṇā desā,",
  "anya-e1008n:381_0": "Cārittāvaraṇā thiyo.",
  "anya-e1008n:382_0": "77.",
  "anya-e1008n:383_0": "Ghatakumbhasamā nārī,",
  "anya-e1008n:384_0": "Tattaṅgāra samo pumā;",
  "anya-e1008n:385_0": "Tasmā ghatañca aggiñca,",
  "anya-e1008n:386_0": "Nekatra thapaye budho.",
  "anya-e1008n:387_0": "78.",
  "anya-e1008n:388_0": "Āhāro dviguṇo thīnaṃ,",
  "anya-e1008n:389_0": "Buddhi tāsaṃ catugguṇo;",
  "anya-e1008n:390_0": "Chaguṇo byavasāyo ca,",
  "anya-e1008n:391_0": "Kāmocaṭṭhaguṇo mato.",
  "anya-e1008n:392_0": "79.",
  "anya-e1008n:393_0": "Jiṇṇamannaṃ pasaṃseyya,",
  "anya-e1008n:394_0": "Bhariyaṃ gatayobbanaṃ;",
  "anya-e1008n:395_0": "Raṇā paccāgataṃ sūraṃ,",
  "anya-e1008n:396_0": "Sassañca gehamāgataṃ.",
  "anya-e1008n:397_0": "80.",
  "anya-e1008n:398_0": "Asantuṭṭhā dvijā naṭṭhā,",
  "anya-e1008n:399_0": "Santuṭṭhāiva pāthivā;",
  "anya-e1008n:400_0": "Salajjā gaṇikā naṭṭhā,",
  "anya-e1008n:401_0": "Nillajjā ca kulitthiyo.",
  "anya-e1008n:402_0": "81.",
  "anya-e1008n:403_0": "Avaṃsapatito rājā,",
  "anya-e1008n:404_0": "Mūḷhaputto ca paṇḍito;",
  "anya-e1008n:405_0": "Adhanena dhanaṃ pāpya,",
  "anya-e1008n:406_0": "Tiṇaṃva maññate janaṃ.",
  "anya-e1008n:407_0": "82.",
  "anya-e1008n:408_0": "Brahmahāpi naro pujjo,",
  "anya-e1008n:409_0": "Yassatthi vipulaṃ dhanaṃ;",
  "anya-e1008n:410_0": "Sasino tulyavaṃsopi,",
  "anya-e1008n:411_0": "Niddhano paribhūyate.",
  "anya-e1008n:412_0": "83.",
  "anya-e1008n:413_0": "Potthakaṭṭhā tu yāvijjā,",
  "anya-e1008n:414_0": "Parahatthagataṃ dhanaṃ;",
  "anya-e1008n:415_0": "Kiccakāle samuppanne,",
  "anya-e1008n:416_0": "Na sāvijjā na taddhanaṃ.",
  "anya-e1008n:417_0": "84.",
  "anya-e1008n:418_0": "Pādapānaṃ bhayaṃ vātā,",
  "anya-e1008n:419_0": "Padmānaṃ sisirā bhayaṃ;",
  "anya-e1008n:420_0": "Pabbatānaṃ vajīramhā,",
  "anya-e1008n:421_0": "Sādhūnaṃ dujjanā bhayaṃ.",
  "anya-e1008n:422_0": "85.",
  "anya-e1008n:423_0": "Paññe niyujjamāne tu,",
  "anya-e1008n:424_0": "Santi rañño tayoguṇā;",
  "anya-e1008n:425_0": "Yaso sagganivāso ca,",
  "anya-e1008n:426_0": "Vipulo ca dhanāgamo.",
  "anya-e1008n:427_0": "86.",
  "anya-e1008n:428_0": "Mūḷhe niyujjamānetu,",
  "anya-e1008n:429_0": "Khattiyassāguṇā tayo;",
  "anya-e1008n:430_0": "Ayaso catthanāso ca,",
  "anya-e1008n:431_0": "Narake gamanaṃ tathā.",
  "anya-e1008n:432_0": "87.",
  "anya-e1008n:433_0": "Bahūmūḷhasaṅghātehi,",
  "anya-e1008n:434_0": "Aññoññapasuvuttibhi;",
  "anya-e1008n:435_0": "Pacchādyante guṇā sabbe,",
  "anya-e1008n:436_0": "Meghehiva divākaro.",
  "anya-e1008n:437_0": "88.",
  "anya-e1008n:438_0": "Yassa khettaṃ nadītīre,",
  "anya-e1008n:439_0": "Bhariyāpi parappiyā;",
  "anya-e1008n:440_0": "Puttassa vinayo natthi,",
  "anya-e1008n:441_0": "Maccureva nasaṃsayo.",
  "anya-e1008n:442_0": "89.",
  "anya-e1008n:443_0": "Asambhābyaṃ navattabbaṃ,",
  "anya-e1008n:444_0": "Paccakkhamapi dissate;",
  "anya-e1008n:445_0": "Silā tarati pānīyaṃ,",
  "anya-e1008n:446_0": "Gītaṃ gāyati vānaro.",
  "anya-e1008n:447_0": "90.",
  "anya-e1008n:448_0": "Subhikkhaṃ kasake niccaṃ,",
  "anya-e1008n:449_0": "Niccaṃ sukha marogike;",
  "anya-e1008n:450_0": "Bhariyā bhattu piyā yassa,",
  "anya-e1008n:451_0": "Tassa niccossavaṃ gahaṃ.",
  "anya-e1008n:452_0": "91.",
  "anya-e1008n:453_0": "Helassa kammanāsāya,",
  "anya-e1008n:454_0": "Buddhināsāya niddhanaṃ;",
  "anya-e1008n:455_0": "Yācanā mānanāsāya,",
  "anya-e1008n:456_0": "Kulanāsāya bhojanaṃ.",
  "anya-e1008n:457_0": "92.",
  "anya-e1008n:458_0": "Sevitabbo mahāvakkho,",
  "anya-e1008n:459_0": "Phalacchāyā samanvito;",
  "anya-e1008n:460_0": "Yadi devā phalaṃ natthi,",
  "anya-e1008n:461_0": "Chāyā kena nivāraye.",
  "anya-e1008n:462_0": "93.",
  "anya-e1008n:463_0": "Paṭhame najjitā vijjā,",
  "anya-e1008n:464_0": "Dutīye najjitaṃ dhanaṃ;",
  "anya-e1008n:465_0": "Tatīye najjitaṃ puññaṃ,",
  "anya-e1008n:466_0": "Catutthe kiṃkarissati.",
  "anya-e1008n:467_0": "94.",
  "anya-e1008n:468_0": "Nadīkūleca ye vakkhā,",
  "anya-e1008n:469_0": "Parahatthagataṃ dhanaṃ;",
  "anya-e1008n:470_0": "Kiccaṃ thīgocaraṃ yassa,",
  "anya-e1008n:471_0": "Sabbaṃ taṃ viphalaṃ bhave.",
  "anya-e1008n:472_0": "95.",
  "anya-e1008n:473_0": "Kudesamāsajja kutotthasañcayo,",
  "anya-e1008n:474_0": "Kuputtamāsajja kuto jalañjalī;",
  "anya-e1008n:475_0": "Kugehiniṃ pāpya gahe kuto sukhaṃ,",
  "anya-e1008n:476_0": "Kusissamajjhāpayato kuto yaso.",
  "anya-e1008n:477_0": "96.",
  "anya-e1008n:478_0": "Kūpodakaṃ vaṭacchāyā,",
  "anya-e1008n:479_0": "Sāmā thīciṭṭhakālayaṃ;",
  "anya-e1008n:480_0": "Sītakāle bhave uṇhaṃ,",
  "anya-e1008n:481_0": "Gimhakāle ca sītalaṃ.",
  "anya-e1008n:482_0": "97.",
  "anya-e1008n:483_0": "Visaṃ caṅkamanaṃ rattiṃ,",
  "anya-e1008n:484_0": "Visaṃ raññonukulatā;",
  "anya-e1008n:485_0": "Visaṃ thīpi aññāsattā,",
  "anya-e1008n:486_0": "Visaṃ byādhi avekkhito.",
  "anya-e1008n:487_0": "98.",
  "anya-e1008n:488_0": "Duradhītā visaṃ vijjā,",
  "anya-e1008n:489_0": "Ajiṇṇe bhojanaṃ visaṃ;",
  "anya-e1008n:490_0": "Visaṃ goṭṭhī daliddassa,",
  "anya-e1008n:491_0": "Vuddhassa taruṇī visaṃ.",
  "anya-e1008n:492_0": "99.",
  "anya-e1008n:493_0": "Padose nihato pantho,",
  "anya-e1008n:494_0": "Patitā nihatā thiyo;",
  "anya-e1008n:495_0": "Appabījaṃ hataṃ khettaṃ,",
  "anya-e1008n:496_0": "Bhaccadosā hato pabhū.",
  "anya-e1008n:497_0": "100.",
  "anya-e1008n:498_0": "Hatamasottiyaṃ saddhaṃ,",
  "anya-e1008n:499_0": "Hato yañño tvadakkhiṇo;",
  "anya-e1008n:500_0": "Hatā rūpavatī vañjhā,",
  "anya-e1008n:501_0": "Hataṃ senamanāyakaṃ.",
  "anya-e1008n:502_0": "101.",
  "anya-e1008n:503_0": "Vedavedaṅga tattañño,",
  "anya-e1008n:504_0": "Japahomaparāyano;",
  "anya-e1008n:505_0": "Āsīvādavacoyutto,",
  "anya-e1008n:506_0": "Esa rājapurohito.",
  "anya-e1008n:507_0": "102.",
  "anya-e1008n:508_0": "Kulasīlaguṇopeto,",
  "anya-e1008n:509_0": "Sabbadhammaparāyano;",
  "anya-e1008n:510_0": "Pavīṇo pesanādyakkho,",
  "anya-e1008n:511_0": "Dhammādyakkho vidhīyate.",
  "anya-e1008n:512_0": "103.",
  "anya-e1008n:513_0": "Ayubbedakatābhyāso,",
  "anya-e1008n:514_0": "Sabbesaṃ piyadassano;",
  "anya-e1008n:515_0": "Ariyasīlaguṇopeto,",
  "anya-e1008n:516_0": "Esa vajjo vidhīyate.",
  "anya-e1008n:517_0": "104.",
  "anya-e1008n:518_0": "Sakiṃdutta gahitattho,",
  "anya-e1008n:519_0": "Lahuhattho jitakkharo;",
  "anya-e1008n:520_0": "Sabbasattha samālokī,",
  "anya-e1008n:521_0": "Pakaṭṭho nāma lekhako.",
  "anya-e1008n:522_0": "105.",
  "anya-e1008n:523_0": "Samattanītisattañño,",
  "anya-e1008n:524_0": "Vāhane pūjitassamo;",
  "anya-e1008n:525_0": "Sūravīraguṇopeto,",
  "anya-e1008n:526_0": "Senādhyakkho vidhīyate.",
  "anya-e1008n:527_0": "106.",
  "anya-e1008n:528_0": "Sucī vākyapaṭuppañño,",
  "anya-e1008n:529_0": "Paracittopalakkhako;",
  "anya-e1008n:530_0": "Dhīro yathāttha vādī ca,",
  "anya-e1008n:531_0": "Esa dūto vidhīyate.",
  "anya-e1008n:532_0": "107.",
  "anya-e1008n:533_0": "Puttanatta guṇopeto,",
  "anya-e1008n:534_0": "Satthañño piṭṭhapācako;",
  "anya-e1008n:535_0": "Sūro ca kathinoceva,",
  "anya-e1008n:536_0": "Sūpakāro sa uccate.",
  "anya-e1008n:537_0": "108.",
  "anya-e1008n:538_0": "Iṅgitā kāratattañño,",
  "anya-e1008n:539_0": "Balavā piyadassano;",
  "anya-e1008n:540_0": "Appamādī sadā dakkho,",
  "anya-e1008n:541_0": "Patihāro sa uccate.",
  "anya-e1008n:542_0": "109.",
  "anya-e1008n:543_0": "Yassa natthi sayaṃ paññā,",
  "anya-e1008n:544_0": "Satthaṃ tassa karoti kiṃ;",
  "anya-e1008n:545_0": "Locanehi vihīnassa,",
  "anya-e1008n:546_0": "Dappaṇo kiṃkarissati.",
  "anya-e1008n:547_0": "110.",
  "anya-e1008n:548_0": "Kiṃkarissanti vattāro,",
  "anya-e1008n:549_0": "Sotaṃ yattha navijjate;",
  "anya-e1008n:550_0": "Naggakapaṇake dese,",
  "anya-e1008n:551_0": "Rajaṇo kiṃkarissati."
}