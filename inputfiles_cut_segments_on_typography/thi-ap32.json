{
  "thi-ap32:0.1_0": "Therīapadāna",
  "thi-ap32:0.2_0": "Khattiyāvagga",
  "thi-ap32:0.3_0": "2 Caturāsītibhikkhunīsahassaapadāna",
  "thi-ap32:1.1_0": "“Cullāsītisahassāni,",
  "thi-ap32:1.2_0": "brāhmaññakulasambhavā;",
  "thi-ap32:1.3_0": "Sukhumālahatthapādā,",
  "thi-ap32:1.4_0": "pure tuyhaṁ mahāmune.",
  "thi-ap32:2.1_0": "Vessasuddakule jātā,",
  "thi-ap32:2.2_0": "devā nāgā ca kinnarā;",
  "thi-ap32:2.3_0": "Cātuddīpā bahū kaññā,",
  "thi-ap32:2.4_0": "pure tuyhaṁ mahāmune.",
  "thi-ap32:3.1_0": "Kāci pabbajitā atthi,",
  "thi-ap32:3.2_0": "sabbadassāvino bahū;",
  "thi-ap32:3.3_0": "Devā ca kinnarā nāgā,",
  "thi-ap32:3.4_0": "phusissanti anāgate.",
  "thi-ap32:4.1_0": "Anubhotvā yasaṁ sabbaṁ,",
  "thi-ap32:4.2_0": "Patvāna sabbasampadā;",
  "thi-ap32:4.3_0": "Tumhaṁ pasādaṁ paṭiladdhā,",
  "thi-ap32:4.4_0": "Bujjhissanti anāgate.",
  "thi-ap32:5.1_0": "Amhe brāhmaṇadhītā tu,",
  "thi-ap32:5.2_0": "brāhmaññakulasambhavā;",
  "thi-ap32:5.3_0": "Pekkhato no mahāvīra,",
  "thi-ap32:5.4_0": "pāde vandāma cakkhuma.",
  "thi-ap32:6.1_0": "Upāhatā bhavā sabbe,",
  "thi-ap32:6.2_0": "mūlataṇhā samūhatā;",
  "thi-ap32:6.3_0": "Samucchinnā anusayā,",
  "thi-ap32:6.4_0": "puññasaṅkhāradālitā.",
  "thi-ap32:7.1_0": "Samādhigocarā sabbā,",
  "thi-ap32:7.2_0": "samāpattivasī katā;",
  "thi-ap32:7.3_0": "Jhānena dhammaratiyā,",
  "thi-ap32:7.4_0": "viharissāma no sadā.",
  "thi-ap32:8.1_0": "Bhavanetti avijjā ca,",
  "thi-ap32:8.2_0": "saṅkhārāpi ca khepitā;",
  "thi-ap32:8.3_0": "Sududdasaṁ padaṁ gantvā,",
  "thi-ap32:8.4_0": "anujānātha nāyaka”.",
  "thi-ap32:9.1_0": "“Upakārā mamaṁ tumhe,",
  "thi-ap32:9.2_0": "dīgharattaṁ katāvino;",
  "thi-ap32:9.3_0": "Catunnaṁ saṁsayaṁ chetvā,",
  "thi-ap32:9.4_0": "sabbā gacchantu nibbutiṁ”.",
  "thi-ap32:10.1_0": "Vanditvā munino pāde,",
  "thi-ap32:10.2_0": "katvā iddhivikubbanaṁ;",
  "thi-ap32:10.3_0": "Kāci dassenti ālokaṁ,",
  "thi-ap32:10.4_0": "andhakāramathāparā.",
  "thi-ap32:11.1_0": "Dassenti candasūriye,",
  "thi-ap32:11.2_0": "sāgarañca samacchakaṁ;",
  "thi-ap32:11.3_0": "Sineruṁ paribhaṇḍañca,",
  "thi-ap32:11.4_0": "dassenti pārichattakaṁ.",
  "thi-ap32:12.1_0": "Tāvatiṁsañca bhavanaṁ,",
  "thi-ap32:12.2_0": "yāmaṁ dassenti iddhiyā;",
  "thi-ap32:12.3_0": "Tusitaṁ nimmite deve,",
  "thi-ap32:12.4_0": "vasavattī mahissare.",
  "thi-ap32:13.1_0": "Brahmāno kāci dassenti,",
  "thi-ap32:13.2_0": "caṅkamañca mahārahaṁ;",
  "thi-ap32:13.3_0": "Brahmavaṇṇañca māpetvā,",
  "thi-ap32:13.4_0": "dhammaṁ desenti suññataṁ.",
  "thi-ap32:14.1_0": "Nānāvikubbanaṁ katvā,",
  "thi-ap32:14.2_0": "iddhiṁ dassiya satthuno;",
  "thi-ap32:14.3_0": "Dassayiṁsu balaṁ sabbā,",
  "thi-ap32:14.4_0": "pāde vandiṁsu satthuno.",
  "thi-ap32:15.1_0": "“Iddhīsu ca vasī homa,",
  "thi-ap32:15.2_0": "dibbāya sotadhātuyā;",
  "thi-ap32:15.3_0": "Cetopariyañāṇassa,",
  "thi-ap32:15.4_0": "vasī homa mahāmune.",
  "thi-ap32:16.1_0": "Pubbenivāsaṁ jānāma,",
  "thi-ap32:16.2_0": "dibbacakkhu visodhitaṁ;",
  "thi-ap32:16.3_0": "Sabbāsavā parikkhīṇā,",
  "thi-ap32:16.4_0": "natthi dāni punabbhavo.",
  "thi-ap32:17.1_0": "Atthadhammaniruttīsu,",
  "thi-ap32:17.2_0": "paṭibhāne tatheva ca;",
  "thi-ap32:17.3_0": "Ñāṇaṁ amhaṁ mahāvīra,",
  "thi-ap32:17.4_0": "uppannaṁ tava santike.",
  "thi-ap32:18.1_0": "Pubbānaṁ lokanāthānaṁ,",
  "thi-ap32:18.2_0": "saṅgamaṁ no nidassitaṁ;",
  "thi-ap32:18.3_0": "Adhikāraṁ bahuṁ amhaṁ,",
  "thi-ap32:18.4_0": "tuyhatthāya mahāmune.",
  "thi-ap32:19.1_0": "Yaṁ amhehi kataṁ kammaṁ,",
  "thi-ap32:19.2_0": "kusalaṁ sara taṁ mune;",
  "thi-ap32:19.3_0": "Tuyhatthāya mahāvīra,",
  "thi-ap32:19.4_0": "puññānupacitāni no.",
  "thi-ap32:20.1_0": "Satasahassito kappe,",
  "thi-ap32:20.2_0": "padumuttaro mahāmuni;",
  "thi-ap32:20.3_0": "Puraṁ haṁsavatī nāma,",
  "thi-ap32:20.4_0": "sambuddhassa kulāsayaṁ.",
  "thi-ap32:21.1_0": "Dvārena haṁsavatiyā,",
  "thi-ap32:21.2_0": "gaṅgā sandati sabbadā;",
  "thi-ap32:21.3_0": "Ubbāḷhā nadiyā bhikkhū,",
  "thi-ap32:21.4_0": "gamanaṁ na labhanti te.",
  "thi-ap32:22.1_0": "Divasaṁ dve tayo ceva,",
  "thi-ap32:22.2_0": "sattāhaṁ māsikaṁ tato;",
  "thi-ap32:22.3_0": "Catumāsampi sampuṇṇaṁ,",
  "thi-ap32:22.4_0": "gamanaṁ na labhanti te.",
  "thi-ap32:23.1_0": "Tadā ahu sattasāro,",
  "thi-ap32:23.2_0": "jaṭilo nāma raṭṭhiko;",
  "thi-ap32:23.3_0": "Oruddhe bhikkhavo disvā,",
  "thi-ap32:23.4_0": "setuṁ gaṅgāya kārayi.",
  "thi-ap32:24.1_0": "Tadā satasahassehi,",
  "thi-ap32:24.2_0": "setuṁ gaṅgāya kārayi;",
  "thi-ap32:24.3_0": "Saṅghassa orime tīre,",
  "thi-ap32:24.4_0": "vihārañca akārayi.",
  "thi-ap32:25.1_0": "Itthiyo purisā ceva,",
  "thi-ap32:25.2_0": "uccanīcakulāni ca;",
  "thi-ap32:25.3_0": "Tassa setuṁ vihārañca,",
  "thi-ap32:25.4_0": "samabhāgaṁ akaṁsu te.",
  "thi-ap32:26.1_0": "Amhe aññe ca mānujā,",
  "thi-ap32:26.2_0": "vippasannena cetasā;",
  "thi-ap32:26.3_0": "Tassa dhammesu dāyādā,",
  "thi-ap32:26.4_0": "nagare janapadesu ca.",
  "thi-ap32:27.1_0": "Itthī pumā kumārā ca,",
  "thi-ap32:27.2_0": "bahū ceva kumārikā;",
  "thi-ap32:27.3_0": "Setuno ca vihārassa,",
  "thi-ap32:27.4_0": "vālukā ākiriṁsu te.",
  "thi-ap32:28.1_0": "Vīthiṁ sammajjanaṁ katvā,",
  "thi-ap32:28.2_0": "kadalīpuṇṇaghaṭe dhaje;",
  "thi-ap32:28.3_0": "Dhūpaṁ cuṇṇañca mālañca,",
  "thi-ap32:28.4_0": "kāraṁ katvāna satthuno.",
  "thi-ap32:29.1_0": "Setuvihāre kāretvā,",
  "thi-ap32:29.2_0": "nimantetvā vināyakaṁ;",
  "thi-ap32:29.3_0": "Mahādānaṁ daditvāna,",
  "thi-ap32:29.4_0": "sambodhiṁ abhipatthayiṁ.",
  "thi-ap32:30.1_0": "Padumuttaro mahāvīro,",
  "thi-ap32:30.2_0": "tārako sabbapāṇinaṁ;",
  "thi-ap32:30.3_0": "Anumodanīyaṅkāsi,",
  "thi-ap32:30.4_0": "jaṭilassa mahāmuni.",
  "thi-ap32:31.1_0": "‘Satasahassātikkante,",
  "thi-ap32:31.2_0": "kappo hessati bhaddako;",
  "thi-ap32:31.3_0": "Bhavābhavenubhotvāna,",
  "thi-ap32:31.4_0": "pāpuṇissati bodhiyaṁ.",
  "thi-ap32:32.1_0": "Kāci hatthaparikammaṁ,",
  "thi-ap32:32.2_0": "katāvī naranāriyo;",
  "thi-ap32:32.3_0": "Anāgatamhi addhāne,",
  "thi-ap32:32.4_0": "sabbā hessanti sammukhā’.",
  "thi-ap32:33.1_0": "Tena kammavipākena,",
  "thi-ap32:33.2_0": "cetanāpaṇidhīhi ca;",
  "thi-ap32:33.3_0": "Uppannā devabhavanaṁ,",
  "thi-ap32:33.4_0": "tuyhaṁ tā paricārikā.",
  "thi-ap32:34.1_0": "Dibbasukhaṁ asaṅkhiyaṁ,",
  "thi-ap32:34.2_0": "mānusañca asaṅkhiyaṁ;",
  "thi-ap32:34.3_0": "Tuyhaṁ te paricārema,",
  "thi-ap32:34.4_0": "saṁsarimha bhavābhave.",
  "thi-ap32:35.1_0": "Satasahassito kappe,",
  "thi-ap32:35.2_0": "sukataṁ kammasampadaṁ;",
  "thi-ap32:35.3_0": "Sukhumālī manussānaṁ,",
  "thi-ap32:35.4_0": "atho devapure vare.",
  "thi-ap32:36.1_0": "Rūpabhogayase ceva,",
  "thi-ap32:36.2_0": "atho kittiñca sakkataṁ;",
  "thi-ap32:36.3_0": "Labhāma satataṁ sabbaṁ,",
  "thi-ap32:36.4_0": "sukataṁ kammasampadaṁ.",
  "thi-ap32:37.1_0": "Pacchime bhave sampatte,",
  "thi-ap32:37.2_0": "jātāmha brāhmaṇe kule;",
  "thi-ap32:37.3_0": "Sukhumālahatthapādā,",
  "thi-ap32:37.4_0": "sakyaputtanivesane.",
  "thi-ap32:38.1_0": "Sabbakālampi pathaviṁ,",
  "thi-ap32:38.2_0": "na passāma na laṅkataṁ;",
  "thi-ap32:38.3_0": "Cikkhallabhūmimasuciṁ,",
  "thi-ap32:38.4_0": "na passāma mahāmune.",
  "thi-ap32:39.1_0": "Agāraṁ vasante amhe,",
  "thi-ap32:39.2_0": "sakkāraṁ sabbakālikaṁ;",
  "thi-ap32:39.3_0": "Upanenti sadā sabbaṁ,",
  "thi-ap32:39.4_0": "pubbakammaphalena no.",
  "thi-ap32:40.1_0": "Agāraṁ pajahitvāna,",
  "thi-ap32:40.2_0": "pabbajitvānagāriyaṁ;",
  "thi-ap32:40.3_0": "Saṁsārapathanitthiṇṇā,",
  "thi-ap32:40.4_0": "vītarāgā bhavāmase.",
  "thi-ap32:41.1_0": "Cīvaraṁ piṇḍapātañca,",
  "thi-ap32:41.2_0": "paccayaṁ sayanāsanaṁ;",
  "thi-ap32:41.3_0": "Upanenti sadā amhe,",
  "thi-ap32:41.4_0": "sahassāni tato tato.",
  "thi-ap32:42.1_0": "Kilesā jhāpitā amhaṁ,",
  "thi-ap32:42.2_0": "…pe…",
  "thi-ap32:42.3_0": "viharāma anāsavā.",
  "thi-ap32:43.1_0": "Svāgataṁ vata no āsi,",
  "thi-ap32:43.2_0": "…pe…",
  "thi-ap32:43.3_0": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap32:44.1_0": "Paṭisambhidā catasso,",
  "thi-ap32:44.2_0": "…pe…",
  "thi-ap32:44.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "thi-ap32:45.1_0": "Itthaṁ sudaṁ caturāsītibrāhmaṇakaññābhikkhunīsahassāni bhagavato sammukhā imā gāthāyo abhāsitthāti.",
  "thi-ap32:46.1_0": "Caturāsītibhikkhunīsahassāpadānaṁ dutiyaṁ."
}