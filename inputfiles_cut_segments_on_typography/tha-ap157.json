{
  "tha-ap157:0.1_0": "Therāpadāna",
  "tha-ap157:0.2_0": "Bandhujīvakavagga",
  "tha-ap157:0.3_0": "5 Mandāravapupphapūjakattheraapadāna",
  "tha-ap157:1.1_0": "“Devaputto ahaṁ santo,",
  "tha-ap157:1.2_0": "pūjayiṁ sikhināyakaṁ;",
  "tha-ap157:1.3_0": "Mandāravena pupphena,",
  "tha-ap157:1.4_0": "buddhassa abhiropayiṁ.",
  "tha-ap157:2.1_0": "Sattāhaṁ chadanaṁ āsi,",
  "tha-ap157:2.2_0": "dibbaṁ mālaṁ tathāgate;",
  "tha-ap157:2.3_0": "Sabbe janā samāgantvā,",
  "tha-ap157:2.4_0": "namassiṁsu tathāgataṁ.",
  "tha-ap157:3.1_0": "Ekattiṁse ito kappe,",
  "tha-ap157:3.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap157:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap157:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap157:4.1_0": "Ito ca dasame kappe,",
  "tha-ap157:4.2_0": "rājāhosiṁ jutindharo;",
  "tha-ap157:4.3_0": "Sattaratanasampanno,",
  "tha-ap157:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap157:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap157:5.2_0": "…pe…",
  "tha-ap157:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap157:6.1_0": "Itthaṁ sudaṁ āyasmā mandāravapupphapūjako thero imā gāthāyo abhāsitthāti.",
  "tha-ap157:7.1_0": "Mandāravapupphapūjakattherassāpadānaṁ pañcamaṁ."
}