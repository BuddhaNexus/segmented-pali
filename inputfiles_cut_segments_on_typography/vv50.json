{
  "vv50:0.1_0": "Vimānavatthu",
  "vv50:0.2_0": "Itthivimāna",
  "vv50:0.3_0": "Mañjiṭṭhakavagga",
  "vv50:0.4_0": "12. Rajjumālāvimānavatthu",
  "vv50:1.1_0": "“Abhikkantena vaṇṇena,",
  "vv50:1.2_0": "yā tvaṁ tiṭṭhasi devate;",
  "vv50:1.3_0": "Hatthapāde ca viggayha,",
  "vv50:1.4_0": "naccasi suppavādite.",
  "vv50:2.1_0": "Tassā te naccamānāya,",
  "vv50:2.2_0": "aṅgamaṅgehi sabbaso;",
  "vv50:2.3_0": "Dibbā saddā niccharanti,",
  "vv50:2.4_0": "savanīyā manoramā.",
  "vv50:3.1_0": "Tassā te naccamānāya,",
  "vv50:3.2_0": "aṅgamaṅgehi sabbaso;",
  "vv50:3.3_0": "Dibbā gandhā pavāyanti,",
  "vv50:3.4_0": "sucigandhā manoramā.",
  "vv50:4.1_0": "Vivattamānā kāyena,",
  "vv50:4.2_0": "yā veṇīsu piḷandhanā;",
  "vv50:4.3_0": "Tesaṁ suyyati nigghoso,",
  "vv50:4.4_0": "tūriye pañcaṅgike yathā.",
  "vv50:5.1_0": "Vaṭaṁsakā vātadhutā,",
  "vv50:5.2_0": "vātena sampakampitā;",
  "vv50:5.3_0": "Tesaṁ suyyati nigghoso,",
  "vv50:5.4_0": "tūriye pañcaṅgike yathā.",
  "vv50:6.1_0": "Yāpi te sirasmiṁ mālā,",
  "vv50:6.2_0": "sucigandhā manoramā;",
  "vv50:6.3_0": "Vāti gandho disā sabbā,",
  "vv50:6.4_0": "rukkho mañjūsako yathā.",
  "vv50:7.1_0": "Ghāyase taṁ sucigandhaṁ,",
  "vv50:7.2_0": "rūpaṁ passasi amānusaṁ;",
  "vv50:7.3_0": "Devate pucchitācikkha,",
  "vv50:7.4_0": "kissa kammassidaṁ phalan”ti.",
  "vv50:8.1_0": "“Dāsī ahaṁ pure āsiṁ,",
  "vv50:8.2_0": "gayāyaṁ brāhmaṇassahaṁ;",
  "vv50:8.3_0": "Appapuññā alakkhikā,",
  "vv50:8.4_0": "rajjumālāti maṁ viduṁ.",
  "vv50:9.1_0": "Akkosānaṁ vadhānañca,",
  "vv50:9.2_0": "tajjanāya ca uggatā;",
  "vv50:9.3_0": "Kuṭaṁ gahetvā nikkhamma,",
  "vv50:9.4_0": "agañchiṁ udahāriyā.",
  "vv50:10.1_0": "Vipathe kuṭaṁ nikkhipitvā,",
  "vv50:10.2_0": "vanasaṇḍaṁ upāgamiṁ;",
  "vv50:10.3_0": "Idhevāhaṁ marissāmi,",
  "vv50:10.4_0": "ko attho jīvitena me.",
  "vv50:11.1_0": "Daḷhaṁ pāsaṁ karitvāna,",
  "vv50:11.2_0": "āsumbhitvāna pādape;",
  "vv50:11.3_0": "Tato disā vilokesiṁ,",
  "vv50:11.4_0": "‘ko nu kho vanamassito’.",
  "vv50:12.1_0": "Tatthaddasāsiṁ sambuddhaṁ,",
  "vv50:12.2_0": "sabbalokahitaṁ muniṁ;",
  "vv50:12.3_0": "Nisinnaṁ rukkhamūlasmiṁ,",
  "vv50:12.4_0": "jhāyantaṁ akutobhayaṁ.",
  "vv50:13.1_0": "Tassā me ahu saṁvego,",
  "vv50:13.2_0": "abbhuto lomahaṁsano;",
  "vv50:13.3_0": "Ko nu kho vanamassito,",
  "vv50:13.4_0": "manusso udāhu devatā.",
  "vv50:14.1_0": "Pāsādikaṁ pasādanīyaṁ,",
  "vv50:14.2_0": "vanā nibbanamāgataṁ;",
  "vv50:14.3_0": "Disvā mano me pasīdi,",
  "vv50:14.4_0": "nāyaṁ yādisakīdiso.",
  "vv50:15.1_0": "Guttindriyo jhānarato,",
  "vv50:15.2_0": "abahiggatamānaso;",
  "vv50:15.3_0": "Hito sabbassa lokassa,",
  "vv50:15.4_0": "buddho ayaṁ bhavissati.",
  "vv50:16.1_0": "Bhayabheravo durāsado,",
  "vv50:16.2_0": "sīhova guhamassito;",
  "vv50:16.3_0": "Dullabhāyaṁ dassanāya,",
  "vv50:16.4_0": "pupphaṁ odumbaraṁ yathā.",
  "vv50:17.1_0": "So maṁ mudūhi vācāhi,",
  "vv50:17.2_0": "ālapitvā tathāgato;",
  "vv50:17.3_0": "Rajjumāleti maṁvoca,",
  "vv50:17.4_0": "saraṇaṁ gaccha tathāgataṁ.",
  "vv50:18.1_0": "Tāhaṁ giraṁ suṇitvāna,",
  "vv50:18.2_0": "nelaṁ atthavatiṁ suciṁ;",
  "vv50:18.3_0": "Saṇhaṁ muduñca vagguñca,",
  "vv50:18.4_0": "sabbasokāpanūdanaṁ.",
  "vv50:19.1_0": "Kallacittañca maṁ ñatvā,",
  "vv50:19.2_0": "pasannaṁ suddhamānasaṁ;",
  "vv50:19.3_0": "Hito sabbassa lokassa,",
  "vv50:19.4_0": "anusāsi tathāgato.",
  "vv50:20.1_0": "Idaṁ dukkhanti maṁvoca,",
  "vv50:20.2_0": "Ayaṁ dukkhassa sambhavo;",
  "vv50:20.3_0": "Dukkha nirodho maggo ca,",
  "vv50:20.4_0": "Añjaso amatogadho.",
  "vv50:21.1_0": "Anukampakassa kusalassa,",
  "vv50:21.2_0": "Ovādamhi ahaṁ ṭhitā;",
  "vv50:21.3_0": "Ajjhagā amataṁ santiṁ,",
  "vv50:21.4_0": "Nibbānaṁ padamaccutaṁ.",
  "vv50:22.1_0": "Sāhaṁ avaṭṭhitāpemā,",
  "vv50:22.2_0": "dassane avikampinī;",
  "vv50:22.3_0": "Mūlajātāya saddhāya,",
  "vv50:22.4_0": "dhītā buddhassa orasā.",
  "vv50:23.1_0": "Sāhaṁ ramāmi kīḷāmi,",
  "vv50:23.2_0": "modāmi akutobhayā;",
  "vv50:23.3_0": "Dibbamālaṁ dhārayāmi,",
  "vv50:23.4_0": "pivāmi madhumaddavaṁ.",
  "vv50:24.1_0": "Saṭṭhitūriyasahassāni,",
  "vv50:24.2_0": "paṭibodhaṁ karonti me;",
  "vv50:24.3_0": "Āḷambo gaggaro bhīmo,",
  "vv50:24.4_0": "sādhuvādī ca saṁsayo.",
  "vv50:25.1_0": "Pokkharo ca suphasso ca,",
  "vv50:25.2_0": "vīṇāmokkhā ca nāriyo;",
  "vv50:25.3_0": "Nandā ceva sunandā ca,",
  "vv50:25.4_0": "soṇadinnā sucimhitā.",
  "vv50:26.1_0": "Alambusā missakesī ca,",
  "vv50:26.2_0": "puṇḍarīkātidāruṇī;",
  "vv50:26.3_0": "Eṇīphassā suphassā ca,",
  "vv50:26.4_0": "subhaddā muduvādinī.",
  "vv50:27.1_0": "Etā caññā ca seyyāse,",
  "vv50:27.2_0": "accharānaṁ pabodhikā;",
  "vv50:27.3_0": "Tā maṁ kālenupāgantvā,",
  "vv50:27.4_0": "abhibhāsanti devatā.",
  "vv50:28.1_0": "Handa naccāma gāyāma,",
  "vv50:28.2_0": "handa taṁ ramayāmase;",
  "vv50:28.3_0": "Nayidaṁ akatapuññānaṁ,",
  "vv50:28.4_0": "katapuññānamevidaṁ.",
  "vv50:29.1_0": "Asokaṁ nandanaṁ rammaṁ,",
  "vv50:29.2_0": "tidasānaṁ mahāvanaṁ;",
  "vv50:29.3_0": "Sukhaṁ akatapuññānaṁ,",
  "vv50:29.4_0": "idha natthi parattha ca.",
  "vv50:30.1_0": "Sukhañca katapuññānaṁ,",
  "vv50:30.2_0": "idha ceva parattha ca;",
  "vv50:30.3_0": "Tesaṁ sahabyakāmānaṁ,",
  "vv50:30.4_0": "kattabbaṁ kusalaṁ bahuṁ;",
  "vv50:30.5_0": "Katapuññā hi modanti,",
  "vv50:30.6_0": "sagge bhogasamaṅgino.",
  "vv50:31.1_0": "Bahūnaṁ vata atthāya,",
  "vv50:31.2_0": "uppajjanti tathāgatā;",
  "vv50:31.3_0": "Dakkhiṇeyyā manussānaṁ,",
  "vv50:31.4_0": "puññakhettānamākarā;",
  "vv50:31.5_0": "Yattha kāraṁ karitvāna,",
  "vv50:31.6_0": "sagge modanti dāyakā”ti.",
  "vv50:32.1_0": "Rajjumālāvimānaṁ dvādasamaṁ.",
  "vv50:33.1_0": "Tassuddānaṁ",
  "vv50:34.1_0": "Mañjiṭṭhā pabhassarā nāgā,",
  "vv50:34.2_0": "Alomākañjikadāyikā;",
  "vv50:34.3_0": "Vihāracaturitthambā,",
  "vv50:34.4_0": "Pītā ucchuvandanarajjumālā ca;",
  "vv50:34.5_0": "Vaggo tena pavuccatīti.",
  "vv50:35.1_0": "Mañjiṭṭhakavaggo catuttho.",
  "vv50:36.1_0": "Itthivimānaṁ samattaṁ."
}