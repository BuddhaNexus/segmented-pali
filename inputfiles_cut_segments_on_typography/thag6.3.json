{
  "thag6.3:0.1_0": "Theragāthā 6.3",
  "thag6.3:0.2_0": "Chakkanipāta",
  "thag6.3:0.3_0": "Paṭhamavagga",
  "thag6.3:0.4_0": "Mahānāgattheragāthā",
  "thag6.3:1.1_0": "“Yassa sabrahmacārīsu,",
  "thag6.3:1.2_0": "gāravo nūpalabbhati;",
  "thag6.3:1.3_0": "Parihāyati saddhammā,",
  "thag6.3:1.4_0": "maccho appodake yathā.",
  "thag6.3:2.1_0": "Yassa sabrahmacārīsu,",
  "thag6.3:2.2_0": "gāravo nūpalabbhati;",
  "thag6.3:2.3_0": "Na virūhati saddhamme,",
  "thag6.3:2.4_0": "khette bījaṁva pūtikaṁ.",
  "thag6.3:3.1_0": "Yassa sabrahmacārīsu,",
  "thag6.3:3.2_0": "gāravo nūpalabbhati;",
  "thag6.3:3.3_0": "Ārakā hoti nibbānā,",
  "thag6.3:3.4_0": "dhammarājassa sāsane.",
  "thag6.3:4.1_0": "Yassa sabrahmacārīsu,",
  "thag6.3:4.2_0": "gāravo upalabbhati;",
  "thag6.3:4.3_0": "Na vihāyati saddhammā,",
  "thag6.3:4.4_0": "maccho bavhodake yathā.",
  "thag6.3:5.1_0": "Yassa sabrahmacārīsu,",
  "thag6.3:5.2_0": "gāravo upalabbhati;",
  "thag6.3:5.3_0": "So virūhati saddhamme,",
  "thag6.3:5.4_0": "khette bījaṁva bhaddakaṁ.",
  "thag6.3:6.1_0": "Yassa sabrahmacārīsu,",
  "thag6.3:6.2_0": "gāravo upalabbhati;",
  "thag6.3:6.3_0": "Santike hoti nibbānaṁ,",
  "thag6.3:6.4_0": "dhammarājassa sāsane”ti.",
  "thag6.3:6.5_0": "… Mahānāgo thero …."
}