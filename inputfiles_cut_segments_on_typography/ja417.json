{
  "ja417:0.1_0": "Jātaka",
  "ja417:0.2_0": "Aṭṭhakanipāta",
  "ja417:0.3_0": "Kaccānivagga",
  "ja417:0.4_0": "1. Kaccānijātaka",
  "ja417:1.1_0": "“Odātavatthā suci allakesā,",
  "ja417:1.2_0": "Kaccāni kiṁ kumbhimadhissayitvā;",
  "ja417:1.3_0": "Piṭṭhā tilā dhovasi taṇḍulāni,",
  "ja417:1.4_0": "Tilodano hehiti kissahetu”.",
  "ja417:2.1_0": "“Na kho ayaṁ brāhmaṇa bhojanatthā,",
  "ja417:2.2_0": "Tilodano hehiti sādhupakko;",
  "ja417:2.3_0": "Dhammo mato tassa pahuttamajja,",
  "ja417:2.4_0": "Ahaṁ karissāmi susānamajjhe”.",
  "ja417:3.1_0": "“Anuvicca kaccāni karohi kiccaṁ,",
  "ja417:3.2_0": "Dhammo mato ko nu taveva saṁsi;",
  "ja417:3.3_0": "Sahassanetto atulānubhāvo,",
  "ja417:3.4_0": "Na miyyatī dhammavaro kadāci”.",
  "ja417:4.1_0": "“Daḷhappamāṇaṁ mama ettha brahme,",
  "ja417:4.2_0": "Dhammo mato natthi mamettha kaṅkhā;",
  "ja417:4.3_0": "Yeyeva dāni pāpā bhavanti,",
  "ja417:4.4_0": "Te teva dāni sukhitā bhavanti.",
  "ja417:5.1_0": "Suṇisā hi mayhaṁ vañjhā ahosi,",
  "ja417:5.2_0": "Sā maṁ vadhitvāna vijāyi puttaṁ;",
  "ja417:5.3_0": "Sā dāni sabbassa kulassa issarā,",
  "ja417:5.4_0": "Ahaṁ panamhi apaviddhā ekikā”.",
  "ja417:6.1_0": "“Jīvāmi vohaṁ na matohamasmi,",
  "ja417:6.2_0": "Taveva atthāya idhāgatosmi;",
  "ja417:6.3_0": "Yā taṁ vadhitvāna vijāyi puttaṁ,",
  "ja417:6.4_0": "Sahāva puttena karomi bhasmaṁ”.",
  "ja417:7.1_0": "“Evañca te ruccati devarāja,",
  "ja417:7.2_0": "Mameva atthāya idhāgatosi;",
  "ja417:7.3_0": "Ahañca putto suṇisā ca nattā,",
  "ja417:7.4_0": "Sammodamānā gharamāvasema”.",
  "ja417:8.1_0": "“Evañca te ruccati kātiyāni,",
  "ja417:8.2_0": "Hatāpi santā na jahāsi dhammaṁ;",
  "ja417:8.3_0": "Tuvañca putto suṇisā ca nattā,",
  "ja417:8.4_0": "Sammodamānā gharamāvasetha.",
  "ja417:9.1_0": "Sā kātiyānī suṇisāya saddhiṁ,",
  "ja417:9.2_0": "Sammodamānā gharamāvasittha;",
  "ja417:9.3_0": "Putto ca nattā ca upaṭṭhahiṁsu,",
  "ja417:9.4_0": "Devānamindena adhiggahītā”ti.",
  "ja417:10.1_0": "Kaccānijātakaṁ paṭhamaṁ."
}