{
  "tha-ap97:0.1_0": "Therāpadāna",
  "tha-ap97:0.2_0": "Sudhāvagga",
  "tha-ap97:0.3_0": "5. Gandhamāliyattheraapadāna",
  "tha-ap97:1.1_0": "“Siddhatthassa bhagavato,",
  "tha-ap97:1.2_0": "gandhathūpaṁ akāsahaṁ;",
  "tha-ap97:1.3_0": "Sumanehi paṭicchannaṁ,",
  "tha-ap97:1.4_0": "buddhānucchavikaṁ kataṁ.",
  "tha-ap97:2.1_0": "Kañcanagghiyasaṅkāsaṁ,",
  "tha-ap97:2.2_0": "buddhaṁ lokagganāyakaṁ;",
  "tha-ap97:2.3_0": "Indīvaraṁva jalitaṁ,",
  "tha-ap97:2.4_0": "ādittaṁva hutāsanaṁ.",
  "tha-ap97:3.1_0": "Byagghūsabhaṁva pavaraṁ,",
  "tha-ap97:3.2_0": "abhijātaṁva kesariṁ;",
  "tha-ap97:3.3_0": "Nisinnaṁ samaṇānaggaṁ,",
  "tha-ap97:3.4_0": "bhikkhusaṅghapurakkhataṁ.",
  "tha-ap97:4.1_0": "Vanditvā satthuno pāde,",
  "tha-ap97:4.2_0": "pakkāmiṁ uttarāmukho;",
  "tha-ap97:4.3_0": "Catunnavutito kappe,",
  "tha-ap97:4.4_0": "gandhamālaṁ yato adaṁ.",
  "tha-ap97:5.1_0": "Buddhe katassa kārassa,",
  "tha-ap97:5.2_0": "phalenāhaṁ visesato;",
  "tha-ap97:5.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap97:5.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap97:6.1_0": "Cattārīsamhi ekūne,",
  "tha-ap97:6.2_0": "kappe āsiṁsu soḷasa;",
  "tha-ap97:6.3_0": "Devagandhasanāmā te,",
  "tha-ap97:6.4_0": "rājāno cakkavattino.",
  "tha-ap97:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap97:7.2_0": "…pe…",
  "tha-ap97:7.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap97:8.1_0": "Itthaṁ sudaṁ āyasmā gandhamāliyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap97:9.1_0": "Gandhamāliyattherassāpadānaṁ pañcamaṁ."
}