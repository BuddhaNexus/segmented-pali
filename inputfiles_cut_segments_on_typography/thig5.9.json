{
  "thig5.9:0.1_0": "Therīgāthā 5.9",
  "thig5.9:0.2_0": "Pañcakanipāta",
  "thig5.9:0.3_0": "Bhaddākuṇḍalakesātherīgāthā",
  "thig5.9:1.1_0": "“Lūnakesī paṅkadharī,",
  "thig5.9:1.2_0": "ekasāṭī pure cariṁ;",
  "thig5.9:1.3_0": "Avajje vajjamatinī,",
  "thig5.9:1.4_0": "vajje cāvajjadassinī.",
  "thig5.9:2.1_0": "Divāvihārā nikkhamma,",
  "thig5.9:2.2_0": "gijjhakūṭamhi pabbate;",
  "thig5.9:2.3_0": "Addasaṁ virajaṁ buddhaṁ,",
  "thig5.9:2.4_0": "bhikkhusaṅghapurakkhataṁ.",
  "thig5.9:3.1_0": "Nihacca jāṇuṁ vanditvā,",
  "thig5.9:3.2_0": "sammukhā añjaliṁ akaṁ;",
  "thig5.9:3.3_0": "‘Ehi bhadde’ti maṁ avaca,",
  "thig5.9:3.4_0": "sā me āsūpasampadā.",
  "thig5.9:4.1_0": "Ciṇṇā aṅgā ca magadhā,",
  "thig5.9:4.2_0": "vajjī kāsī ca kosalā;",
  "thig5.9:4.3_0": "Anaṇā paṇṇāsa vassāni,",
  "thig5.9:4.4_0": "raṭṭhapiṇḍaṁ abhuñjahaṁ.",
  "thig5.9:5.1_0": "Puññaṁ vata pasavi bahuṁ,",
  "thig5.9:5.2_0": "Sappañño vatāyaṁ upāsako;",
  "thig5.9:5.3_0": "Yo bhaddāya cīvaraṁ adāsi,",
  "thig5.9:5.4_0": "Vippamuttāya sabbaganthehī”ti.",
  "thig5.9:6.1_0": "… Bhaddā kuṇḍalakesā therī …."
}