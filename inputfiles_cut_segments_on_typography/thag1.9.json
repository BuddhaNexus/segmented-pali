{
  "thag1.9:0.1_0": "Theragāthā 1.9",
  "thag1.9:0.2_0": "Ekakanipāta",
  "thag1.9:0.3_0": "Paṭhamavagga",
  "thag1.9:0.4_0": "Pilindavacchattheragāthā",
  "thag1.9:1.1_0": "“Svāgataṁ na durāgataṁ,",
  "thag1.9:1.2_0": "Nayidaṁ dumantitaṁ mama;",
  "thag1.9:1.3_0": "Saṁvibhattesu dhammesu,",
  "thag1.9:1.4_0": "Yaṁ seṭṭhaṁ tadupāgamin”ti.",
  "thag1.9:1.5_0": "Itthaṁ sudaṁ āyasmā pilindavaccho thero gāthaṁ abhāsitthāti."
}