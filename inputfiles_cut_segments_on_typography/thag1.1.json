{
  "thag1.1:0.1_0": "Theragāthā 1.1",
  "thag1.1:0.2_0": "Ekakanipāta",
  "thag1.1:0.3_0": "Paṭhamavagga",
  "thag1.1:0.4_0": "Subhūtittheragāthā",
  "thag1.1:1.0.1_0": "Nidānagāthā",
  "thag1.1:1.0.2_0": "Namo tassa Bhagavato Arahato Sammāsambuddhassa.",
  "thag1.1:1.1_0": "Sīhānaṁva nadantānaṁ,",
  "thag1.1:1.2_0": "dāṭhīnaṁ girigabbhare;",
  "thag1.1:1.3_0": "Suṇātha bhāvitattānaṁ,",
  "thag1.1:1.4_0": "gāthā atthūpanāyikā.",
  "thag1.1:2.1_0": "Yathānāmā yathāgottā,",
  "thag1.1:2.2_0": "yathādhammavihārino;",
  "thag1.1:2.3_0": "Yathādhimuttā sappaññā,",
  "thag1.1:2.4_0": "vihariṁsu atanditā.",
  "thag1.1:3.1_0": "Tattha tattha vipassitvā,",
  "thag1.1:3.2_0": "phusitvā accutaṁ padaṁ;",
  "thag1.1:3.3_0": "Katantaṁ paccavekkhantā,",
  "thag1.1:3.4_0": "imamatthamabhāsisuṁ.",
  "thag1.1:4.0_0": "Subhūtittheragāthā",
  "thag1.1:4.1_0": "“Channā me kuṭikā sukhā nivātā,",
  "thag1.1:4.2_0": "Vassa deva yathāsukhaṁ;",
  "thag1.1:4.3_0": "Cittaṁ me susamāhitaṁ vimuttaṁ,",
  "thag1.1:4.4_0": "Ātāpī viharāmi vassa devā”ti.",
  "thag1.1:4.5_0": "Itthaṁ sudaṁ āyasmā subhūtitthero gāthaṁ abhāsitthāti."
}