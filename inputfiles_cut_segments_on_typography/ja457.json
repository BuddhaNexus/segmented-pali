{
  "ja457:0.1_0": "Jātaka",
  "ja457:0.2_0": "Ekādasakanipāta",
  "ja457:0.3_0": "Mātuposakavagga",
  "ja457:0.4_0": "3. Dhammadevaputtajātaka",
  "ja457:1.1_0": "“Yasokaro puññakarohamasmi,",
  "ja457:1.2_0": "Sadātthuto samaṇabrāhmaṇānaṁ;",
  "ja457:1.3_0": "Maggāraho devamanussapūjito,",
  "ja457:1.4_0": "Dhammo ahaṁ dehi adhamma maggaṁ”.",
  "ja457:2.1_0": "“Adhammayānaṁ daḷhamāruhitvā,",
  "ja457:2.2_0": "Asantasanto balavāhamasmi;",
  "ja457:2.3_0": "Sa kissa hetumhi tavajja dajjaṁ,",
  "ja457:2.4_0": "Maggaṁ ahaṁ dhamma adinnapubbaṁ”.",
  "ja457:3.1_0": "“Dhammo have pāturahosi pubbe,",
  "ja457:3.2_0": "Pacchā adhammo udapādi loke;",
  "ja457:3.3_0": "Jeṭṭho ca seṭṭho ca sanantano ca,",
  "ja457:3.4_0": "Uyyāhi jeṭṭhassa kaniṭṭha maggā”.",
  "ja457:4.1_0": "“Na yācanāya napi pātirūpā,",
  "ja457:4.2_0": "Na arahatā tehaṁ dadeyyaṁ maggaṁ;",
  "ja457:4.3_0": "Yuddhañca no hotu ubhinnamajja,",
  "ja457:4.4_0": "Yuddhamhi yo jessati tassa maggo”.",
  "ja457:5.1_0": "“Sabbā disā anuvisaṭohamasmi,",
  "ja457:5.2_0": "Mahabbalo amitayaso atulyo;",
  "ja457:5.3_0": "Guṇehi sabbehi upetarūpo,",
  "ja457:5.4_0": "Dhammo adhamma tvaṁ kathaṁ vijessasi.",
  "ja457:6.1_0": "Lohena ve haññati jātarūpaṁ,",
  "ja457:6.2_0": "Na jātarūpena hananti lohaṁ;",
  "ja457:6.3_0": "Sace adhammo hañchati dhammamajja,",
  "ja457:6.4_0": "Ayo suvaṇṇaṁ viya dassaneyyaṁ.",
  "ja457:7.1_0": "Sace tuvaṁ yuddhabalo adhamma,",
  "ja457:7.2_0": "Na tuyha vuḍḍhā ca garū ca atthi;",
  "ja457:7.3_0": "Maggañca te dammi piyāppiyena,",
  "ja457:7.4_0": "Vācāduruttānipi te khamāmi”.",
  "ja457:8.1_0": "“Idañca sutvā vacanaṁ adhammo,",
  "ja457:8.2_0": "Avaṁsiro patito uddhapādo;",
  "ja457:8.3_0": "‘Yuddhatthiko ce na labhāmi yuddhaṁ’,",
  "ja457:8.4_0": "Ettāvatā hoti hato adhammo.",
  "ja457:9.1_0": "Khantībalo yuddhabalaṁ vijetvā,",
  "ja457:9.2_0": "Hantvā adhammaṁ nihanitva bhūmyā;",
  "ja457:9.3_0": "Pāyāsi vitto abhiruyha sandanaṁ,",
  "ja457:9.4_0": "Maggeneva atibalo saccanikkamo.",
  "ja457:10.1_0": "Mātā pitā samaṇabrāhmaṇā ca,",
  "ja457:10.2_0": "Asammānitā yassa sake agāre;",
  "ja457:10.3_0": "Idheva nikkhippa sarīradehaṁ,",
  "ja457:10.4_0": "Kāyassa bhedā nirayaṁ vajanti te;",
  "ja457:10.5_0": "Yathā adhammo patito avaṁsiro.",
  "ja457:11.1_0": "Mātā pitā samaṇabrāhmaṇā ca,",
  "ja457:11.2_0": "Susammānitā yassa sake agāre;",
  "ja457:11.3_0": "Idheva nikkhippa sarīradehaṁ,",
  "ja457:11.4_0": "Kāyassa bhedā sugatiṁ vajanti te;",
  "ja457:11.5_0": "Yathāpi dhammo abhiruyha sandanan”ti.",
  "ja457:12.1_0": "Dhammadevaputtajātakaṁ tatiyaṁ."
}