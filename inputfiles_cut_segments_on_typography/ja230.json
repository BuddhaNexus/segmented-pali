{
  "ja230:0.1_0": "Jātaka",
  "ja230:0.2_0": "Dukanipāta",
  "ja230:0.3_0": "Kāsāvavagga",
  "ja230:0.4_0": "10. Dutiyapalāyitajātaka",
  "ja230:1.1_0": "“Dhajamaparimitaṁ anantapāraṁ,",
  "ja230:1.2_0": "Duppasahaṁ dhaṅkehi sāgaraṁva;",
  "ja230:1.3_0": "Girimiva anilena duppasayho,",
  "ja230:1.4_0": "Duppasaho ahamajjatādisena”.",
  "ja230:2.1_0": "“Mā bāliyaṁ vilapi na hissa tādisaṁ,",
  "ja230:2.2_0": "Viḍayhase na hi labhase nisedhakaṁ;",
  "ja230:2.3_0": "Āsajjasi gajamiva ekacārinaṁ,",
  "ja230:2.4_0": "Yo taṁ padā naḷamiva pothayissatī”ti.",
  "ja230:3.1_0": "Dutiyapalāyitajātakaṁ dasamaṁ.",
  "ja230:3.2_0": "Kāsāvavaggo aṭṭhamo.",
  "ja230:4.1_0": "Tassuddānaṁ",
  "ja230:5.1_0": "Varavatthavaco dumakhīṇaphalaṁ,",
  "ja230:5.2_0": "Caturodhammavaraṁ purisuttama;",
  "ja230:5.3_0": "Dhaṅkamagadhā ca tayogirināma,",
  "ja230:5.4_0": "Gajaggavaro dhajavarena dasāti."
}