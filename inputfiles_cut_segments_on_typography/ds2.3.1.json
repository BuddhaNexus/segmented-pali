{
  "ds2.3.1:0.1_0": "Dhammasaṅgaṇī",
  "ds2.3.1:0.2_0": "2 Niddesa",
  "ds2.3.1:0.3_0": "2.3 Nikkhepakaṇḍa",
  "ds2.3.1:0.4_0": "2.3.1. Tikanikkhepa",
  "ds2.3.1:1.0_0": "2.3.1.1. Kusalattika",
  "ds2.3.1:1.1_0": "Katame dhammā kusalā?",
  "ds2.3.1:1.2_0": "Tīṇi kusalamūlāni—",
  "ds2.3.1:1.3_0": "alobho, adoso, amoho;",
  "ds2.3.1:1.4_0": "taṁsampayutto vedanākkhandho, saññākkhandho, saṅkhārakkhandho, viññāṇakkhandho;",
  "ds2.3.1:1.5_0": "taṁsamuṭṭhānaṁ kāyakammaṁ, vacīkammaṁ, manokammaṁ—",
  "ds2.3.1:1.6_0": "ime dhammā kusalā.",
  "ds2.3.1:2.1_0": "Katame dhammā akusalā?",
  "ds2.3.1:2.2_0": "Tīṇi akusalamūlāni—",
  "ds2.3.1:2.3_0": "lobho, doso, moho;",
  "ds2.3.1:2.4_0": "tadekaṭṭhā ca kilesā;",
  "ds2.3.1:2.5_0": "taṁsampayutto vedanākkhandho, saññākkhandho, saṅkhārakkhandho, viññāṇakkhandho;",
  "ds2.3.1:2.6_0": "taṁsamuṭṭhānaṁ kāyakammaṁ, vacīkammaṁ, manokammaṁ—",
  "ds2.3.1:2.7_0": "ime dhammā akusalā.",
  "ds2.3.1:3.1_0": "Katame dhammā abyākatā?",
  "ds2.3.1:3.2_0": "Kusalākusalānaṁ dhammānaṁ vipākā kāmāvacarā, rūpāvacarā, arūpāvacarā, apariyāpannā;",
  "ds2.3.1:3.3_0": "vedanākkhandho, saññākkhandho, saṅkhārakkhandho, viññāṇakkhandho;",
  "ds2.3.1:3.4_0": "ye ca dhammā kiriyā neva kusalā nākusalā na ca kammavipākā;",
  "ds2.3.1:3.5_0": "sabbañca rūpaṁ, asaṅkhatā ca dhātu—",
  "ds2.3.1:3.6_0": "ime dhammā abyākatā.",
  "ds2.3.1:4.0_0": "2.3.1.2. Vedanāttika",
  "ds2.3.1:4.1_0": "Katame dhammā sukhāya vedanāya sampayuttā?",
  "ds2.3.1:4.2_0": "Sukhabhūmiyaṁ kāmāvacare, rūpāvacare, apariyāpanne, sukhaṁ vedanaṁ ṭhapetvā;",
  "ds2.3.1:4.3_0": "taṁsampayutto saññākkhandho, saṅkhārakkhandho, viññāṇakkhandho—",
  "ds2.3.1:4.4_0": "ime dhammā sukhāya vedanāya sampayuttā.",
  "ds2.3.1:5.1_0": "Katame dhammā dukkhāya vedanāya sampayuttā?",
  "ds2.3.1:5.2_0": "Dukkhabhūmiyaṁ kāmāvacare, dukkhaṁ vedanaṁ ṭhapetvā;",
  "ds2.3.1:5.3_0": "taṁsampayutto saññākkhandho, saṅkhārakkhandho, viññāṇakkhandho—",
  "ds2.3.1:5.4_0": "ime dhammā dukkhāya vedanāya sampayuttā.",
  "ds2.3.1:6.1_0": "Katame dhammā adukkhamasukhāya vedanāya sampayuttā?",
  "ds2.3.1:6.2_0": "Adukkhamasukhabhūmiyaṁ kāmāvacare,",
  "ds2.3.1:6.2_1": "rūpāvacare,",
  "ds2.3.1:6.2_2": "arūpāvacare,",
  "ds2.3.1:6.2_3": "apariyāpanne,",
  "ds2.3.1:6.2_4": "adukkhamasukhaṁ vedanaṁ ṭhapetvā;",
  "ds2.3.1:6.3_0": "taṁsampayutto saññākkhandho, saṅkhārakkhandho, viññāṇakkhandho—",
  "ds2.3.1:6.4_0": "ime dhammā adukkhamasukhāya vedanāya sampayuttā.",
  "ds2.3.1:7.0_0": "2.3.1.3. Vipākattika",
  "ds2.3.1:7.1_0": "Katame dhammā vipākā?",
  "ds2.3.1:7.2_0": "Kusalākusalānaṁ dhammānaṁ vipākā kāmāvacarā, rūpāvacarā, arūpāvacarā, apariyāpannā;",
  "ds2.3.1:7.3_0": "vedanākkhandho …pe… viññāṇakkhandho—",
  "ds2.3.1:7.4_0": "ime dhammā vipākā.",
  "ds2.3.1:8.1_0": "Katame dhammā vipākadhammadhammā?",
  "ds2.3.1:8.2_0": "Kusalākusalā dhammā kāmāvacarā, rūpāvacarā, arūpāvacarā, apariyāpannā;",
  "ds2.3.1:8.3_0": "vedanākkhandho …pe… viññāṇakkhandho—",
  "ds2.3.1:8.4_0": "ime dhammā vipākadhammadhammā.",
  "ds2.3.1:9.1_0": "Katame dhammā nevavipākanavipākadhammadhammā?",
  "ds2.3.1:9.2_0": "Ye ca dhammā kiriyā neva kusalā nākusalā na ca kammavipākā, sabbañca rūpaṁ, asaṅkhatā ca dhātu—",
  "ds2.3.1:9.3_0": "ime dhammā nevavipākanavipākadhammadhammā.",
  "ds2.3.1:10.0_0": "2.3.1.4. Upādinnattika",
  "ds2.3.1:10.1_0": "Katame dhammā upādinnupādāniyā?",
  "ds2.3.1:10.2_0": "Sāsavā kusalākusalānaṁ dhammānaṁ vipākā kāmāvacarā, rūpāvacarā, arūpāvacarā;",
  "ds2.3.1:10.3_0": "vedanākkhandho …pe… viññāṇakkhandho;",
  "ds2.3.1:10.4_0": "yañca rūpaṁ kammassa katattā—",
  "ds2.3.1:10.5_0": "ime dhammā upādinnupādāniyā.",
  "ds2.3.1:11.1_0": "Katame dhammā anupādinnupādāniyā?",
  "ds2.3.1:11.2_0": "Sāsavā kusalākusalā dhammā kāmāvacarā, rūpāvacarā, arūpāvacarā;",
  "ds2.3.1:11.3_0": "vedanākkhandho …pe… viññāṇakkhandho;",
  "ds2.3.1:11.4_0": "ye ca dhammā kiriyā neva kusalā nākusalā na ca kammavipākā;",
  "ds2.3.1:11.5_0": "yañca rūpaṁ na kammassa katattā—",
  "ds2.3.1:11.6_0": "ime dhammā anupādinnupādāniyā.",
  "ds2.3.1:12.1_0": "Katame dhammā anupādinnaanupādāniyā?",
  "ds2.3.1:12.2_0": "Apariyāpannā maggā ca, maggaphalāni ca, asaṅkhatā ca dhātu—",
  "ds2.3.1:12.3_0": "ime dhammā anupādinnaanupādāniyā.",
  "ds2.3.1:13.0_0": "2.3.1.5. Saṅkiliṭṭhattika",
  "ds2.3.1:13.1_0": "Katame dhammā saṅkiliṭṭhasaṅkilesikā?",
  "ds2.3.1:13.2_0": "Tīṇi akusalamūlāni—",
  "ds2.3.1:13.3_0": "lobho, doso, moho;",
  "ds2.3.1:13.4_0": "tadekaṭṭhā ca kilesā;",
  "ds2.3.1:13.5_0": "taṁsampayutto vedanākkhandho …pe… viññāṇakkhandho;",
  "ds2.3.1:13.6_0": "taṁsamuṭṭhānaṁ kāyakammaṁ, vacīkammaṁ, manokammaṁ—",
  "ds2.3.1:13.7_0": "ime dhammā saṅkiliṭṭhasaṅkilesikā.",
  "ds2.3.1:14.1_0": "Katame dhammā asaṅkiliṭṭhasaṅkilesikā?",
  "ds2.3.1:14.2_0": "Sāsavā kusalābyākatā dhammā kāmāvacarā, rūpāvacarā, arūpāvacarā;",
  "ds2.3.1:14.3_0": "rūpakkhandho, vedanākkhandho, saññākkhandho, saṅkhārakkhandho, viññāṇakkhandho—",
  "ds2.3.1:14.4_0": "ime dhammā asaṅkiliṭṭhasaṅkilesikā.",
  "ds2.3.1:15.1_0": "Katame dhammā asaṅkiliṭṭhaasaṅkilesikā?",
  "ds2.3.1:15.2_0": "Apariyāpannā maggā ca, maggaphalāni ca, asaṅkhatā ca dhātu—",
  "ds2.3.1:15.3_0": "ime dhammā asaṅkiliṭṭhaasaṅkilesikā.",
  "ds2.3.1:16.0_0": "2.3.1.6. Vitakkattika",
  "ds2.3.1:16.1_0": "Katame dhammā savitakkasavicārā?",
  "ds2.3.1:16.2_0": "Savitakkasavicārabhūmiyaṁ kāmāvacare, rūpāvacare, apariyāpanne, vitakkavicāre ṭhapetvā;",
  "ds2.3.1:16.3_0": "taṁsampayutto vedanākkhandho …pe… viññāṇakkhandho—",
  "ds2.3.1:16.4_0": "ime dhammā savitakkasavicārā.",
  "ds2.3.1:17.1_0": "Katame dhammā avitakkavicāramattā?",
  "ds2.3.1:17.2_0": "Avitakkavicāramattabhūmiyaṁ rūpāvacare, apariyāpanne, vicāraṁ ṭhapetvā;",
  "ds2.3.1:17.3_0": "taṁsampayutto vedanākkhandho …pe… viññāṇakkhandho—",
  "ds2.3.1:17.4_0": "ime dhammā avitakkavicāramattā.",
  "ds2.3.1:18.1_0": "Katame dhammā avitakkaavicārā?",
  "ds2.3.1:18.2_0": "Avitakkaavicārabhūmiyaṁ kāmāvacare, rūpāvacare, arūpāvacare, apariyāpanne;",
  "ds2.3.1:18.3_0": "vedanākkhandho …pe… viññāṇakkhandho;",
  "ds2.3.1:18.4_0": "sabbañca rūpaṁ, asaṅkhatā ca dhātu—",
  "ds2.3.1:18.5_0": "ime dhammā avitakkaavicārā.",
  "ds2.3.1:19.0_0": "2.3.1.7. Pītittika",
  "ds2.3.1:19.1_0": "Katame dhammā pītisahagatā?",
  "ds2.3.1:19.2_0": "Pītibhūmiyaṁ kāmāvacare, rūpāvacare, apariyāpanne, pītiṁ ṭhapetvā;",
  "ds2.3.1:19.3_0": "taṁsampayutto vedanākkhandho …pe… viññāṇakkhandho—",
  "ds2.3.1:19.4_0": "ime dhammā pītisahagatā.",
  "ds2.3.1:20.1_0": "Katame dhammā sukhasahagatā?",
  "ds2.3.1:20.2_0": "Sukhabhūmiyaṁ kāmāvacare, rūpāvacare, apariyāpanne, sukhaṁ ṭhapetvā;",
  "ds2.3.1:20.3_0": "taṁsampayutto saññākkhandho, saṅkhārakkhandho, viññāṇakkhandho—",
  "ds2.3.1:20.4_0": "ime dhammā sukhasahagatā.",
  "ds2.3.1:21.1_0": "Katame dhammā upekkhāsahagatā?",
  "ds2.3.1:21.2_0": "Upekkhābhūmiyaṁ kāmāvacare, rūpāvacare, arūpāvacare, apariyāpanne, upekkhaṁ ṭhapetvā;",
  "ds2.3.1:21.3_0": "taṁsampayutto saññākkhandho, saṅkhārakkhandho, viññāṇakkhandho—",
  "ds2.3.1:21.4_0": "ime dhammā upekkhāsahagatā.",
  "ds2.3.1:22.0_0": "2.3.1.8. Dassanenapahātabbattika",
  "ds2.3.1:22.1_0": "Katame dhammā dassanena pahātabbā?",
  "ds2.3.1:22.2_0": "Tīṇi saññojanāni—",
  "ds2.3.1:22.3_0": "sakkāyadiṭṭhi, vicikicchā, sīlabbataparāmāso.",
  "ds2.3.1:23.1_0": "Tattha katamā sakkāyadiṭṭhi?",
  "ds2.3.1:23.2_0": "Idha assutavā puthujjano ariyānaṁ adassāvī ariyadhammassa akovido ariyadhamme avinīto sappurisānaṁ adassāvī",
  "ds2.3.1:23.2_1": "sappurisadhammassa akovido sappurisadhamme avinīto rūpaṁ attato samanupassati,",
  "ds2.3.1:23.2_2": "rūpavantaṁ vā attānaṁ,",
  "ds2.3.1:23.2_3": "attani vā rūpaṁ,",
  "ds2.3.1:23.2_4": "rūpasmiṁ vā attānaṁ.",
  "ds2.3.1:23.3_0": "Vedanaṁ attato samanupassati, vedanāvantaṁ vā attānaṁ, attani vā vedanaṁ, vedanāya vā attānaṁ.",
  "ds2.3.1:23.4_0": "Saññaṁ attato samanupassati, saññāvantaṁ vā attānaṁ, attani vā saññaṁ, saññāya vā attānaṁ.",
  "ds2.3.1:23.5_0": "Saṅkhāre attato samanupassati, saṅkhāravantaṁ vā attānaṁ, attani vā saṅkhāre, saṅkhāresu vā attānaṁ.",
  "ds2.3.1:23.6_0": "Viññāṇaṁ attato samanupassati, viññāṇavantaṁ vā attānaṁ, attani vā viññāṇaṁ, viññāṇasmiṁ vā attānaṁ.",
  "ds2.3.1:23.7_0": "Yā evarūpā diṭṭhi diṭṭhigataṁ diṭṭhigahanaṁ diṭṭhikantāro diṭṭhivisūkāyikaṁ diṭṭhivipphanditaṁ diṭṭhisaññojanaṁ",
  "ds2.3.1:23.7_1": "gāho paṭiggāho abhiniveso parāmāso kummaggo micchāpatho micchattaṁ titthāyatanaṁ vipariyāsaggāho—",
  "ds2.3.1:23.8_0": "ayaṁ vuccati sakkāyadiṭṭhi.",
  "ds2.3.1:24.1_0": "Tattha katamā vicikicchā?",
  "ds2.3.1:24.2_0": "Satthari kaṅkhati vicikicchati,",
  "ds2.3.1:24.2_1": "dhamme kaṅkhati vicikicchati,",
  "ds2.3.1:24.2_2": "saṅghe kaṅkhati vicikicchati,",
  "ds2.3.1:24.2_3": "sikkhāya kaṅkhati vicikicchati,",
  "ds2.3.1:24.2_4": "pubbante kaṅkhati vicikicchati,",
  "ds2.3.1:24.2_5": "aparante kaṅkhati vicikicchati,",
  "ds2.3.1:24.2_6": "pubbantāparante kaṅkhati vicikicchati,",
  "ds2.3.1:24.2_7": "idappaccayatā paṭiccasamuppannesu dhammesu kaṅkhati vicikicchati.",
  "ds2.3.1:24.3_0": "Yā evarūpā kaṅkhā kaṅkhāyanā kaṅkhāyitattaṁ vimati vicikicchā dveḷhakaṁ dvedhāpatho saṁsayo anekaṁsaggāho",
  "ds2.3.1:24.3_1": "āsappanā parisappanā apariyogāhanā thambhitattaṁ cittassa manovilekho—",
  "ds2.3.1:24.4_0": "ayaṁ vuccati vicikicchā.",
  "ds2.3.1:25.1_0": "Tattha katamo sīlabbataparāmāso?",
  "ds2.3.1:25.2_0": "Ito bahiddhā samaṇabrāhmaṇānaṁ ‘sīlena suddhi,",
  "ds2.3.1:25.2_1": "vatena suddhi,",
  "ds2.3.1:25.2_2": "sīlabbatena suddhī’ti yā evarūpā diṭṭhi diṭṭhigataṁ diṭṭhigahanaṁ diṭṭhikantāro diṭṭhivisūkāyikaṁ diṭṭhivipphanditaṁ",
  "ds2.3.1:25.2_3": "diṭṭhisaññojanaṁ gāho paṭiggāho abhiniveso parāmāso kummaggo micchāpatho micchattaṁ titthāyatanaṁ vipariyāsaggāho—",
  "ds2.3.1:25.3_0": "ayaṁ vuccati sīlabbataparāmāso.",
  "ds2.3.1:26.1_0": "Imāni tīṇi saññojanāni;",
  "ds2.3.1:26.2_0": "tadekaṭṭhā ca kilesā;",
  "ds2.3.1:26.3_0": "taṁsampayutto vedanākkhandho …pe… viññāṇakkhandho;",
  "ds2.3.1:26.4_0": "taṁsamuṭṭhānaṁ kāyakammaṁ vacīkammaṁ manokammaṁ—",
  "ds2.3.1:26.5_0": "ime dhammā dassanena pahātabbā.",
  "ds2.3.1:27.1_0": "Katame dhammā bhāvanāya pahātabbā?",
  "ds2.3.1:27.2_0": "Avaseso lobho, doso, moho;",
  "ds2.3.1:27.3_0": "tadekaṭṭhā ca kilesā;",
  "ds2.3.1:27.4_0": "taṁsampayutto vedanākkhandho …pe… viññāṇakkhandho;",
  "ds2.3.1:27.5_0": "taṁsamuṭṭhānaṁ kāyakammaṁ, vacīkammaṁ, manokammaṁ—",
  "ds2.3.1:27.6_0": "ime dhammā bhāvanāya pahātabbā.",
  "ds2.3.1:28.1_0": "Katame dhammā neva dassanena na bhāvanāya pahātabbā?",
  "ds2.3.1:28.2_0": "Kusalābyākatā dhammā kāmāvacarā, rūpāvacarā, arūpāvacarā, apariyāpannā;",
  "ds2.3.1:28.3_0": "vedanākkhandho …pe… viññāṇakkhandho;",
  "ds2.3.1:28.4_0": "sabbañca rūpaṁ, asaṅkhatā ca dhātu—",
  "ds2.3.1:28.5_0": "ime dhammā neva dassanena na bhāvanāya pahātabbā.",
  "ds2.3.1:29.0_0": "2.3.1.9. Dassanenapahātabbahetukattika",
  "ds2.3.1:29.1_0": "Katame dhammā dassanena pahātabbahetukā?",
  "ds2.3.1:29.2_0": "Tīṇi saññojanāni—",
  "ds2.3.1:29.3_0": "sakkāyadiṭṭhi, vicikicchā, sīlabbataparāmāso.",
  "ds2.3.1:30.1_0": "Tattha katamā sakkāyadiṭṭhi …pe…",
  "ds2.3.1:30.2_0": "ayaṁ vuccati sakkāyadiṭṭhi.",
  "ds2.3.1:31.1_0": "Tattha katamā vicikicchā …pe…",
  "ds2.3.1:31.2_0": "ayaṁ vuccati vicikicchā.",
  "ds2.3.1:32.1_0": "Tattha katamo sīlabbataparāmāso …pe…",
  "ds2.3.1:32.2_0": "ayaṁ vuccati sīlabbataparāmāso.",
  "ds2.3.1:33.1_0": "Imāni tīṇi saññojanāni;",
  "ds2.3.1:33.2_0": "tadekaṭṭhā ca kilesā;",
  "ds2.3.1:33.3_0": "taṁsampayutto vedanākkhandho …pe… viññāṇakkhandho;",
  "ds2.3.1:33.4_0": "taṁsamuṭṭhānaṁ kāyakammaṁ, vacīkammaṁ, manokammaṁ—",
  "ds2.3.1:33.5_0": "ime dhammā dassanena pahātabbahetukā.",
  "ds2.3.1:33.6_0": "Tīṇi saññojanāni—",
  "ds2.3.1:33.7_0": "sakkāyadiṭṭhi, vicikicchā, sīlabbataparāmāso—",
  "ds2.3.1:33.8_0": "ime dhammā dassanena pahātabbā.",
  "ds2.3.1:33.9_0": "Tadekaṭṭho lobho, doso, moho—",
  "ds2.3.1:33.10_0": "ime dhammā dassanena pahātabbahetū.",
  "ds2.3.1:33.11_0": "Tadekaṭṭhā ca kilesā;",
  "ds2.3.1:33.12_0": "taṁsampayutto vedanākkhandho …pe… viññāṇakkhandho;",
  "ds2.3.1:33.13_0": "taṁsamuṭṭhānaṁ kāyakammaṁ, vacīkammaṁ, manokammaṁ—",
  "ds2.3.1:33.14_0": "ime dhammā dassanena pahātabbahetukā.",
  "ds2.3.1:34.1_0": "Katame dhammā bhāvanāya pahātabbahetukā?",
  "ds2.3.1:34.2_0": "Avaseso lobho, doso, moho—",
  "ds2.3.1:34.3_0": "ime dhammā bhāvanāya pahātabbahetū.",
  "ds2.3.1:34.4_0": "Tadekaṭṭhā ca kilesā;",
  "ds2.3.1:34.5_0": "taṁsampayutto vedanākkhandho …pe… viññāṇakkhandho;",
  "ds2.3.1:34.6_0": "taṁsamuṭṭhānaṁ kāyakammaṁ, vacīkammaṁ, manokammaṁ—",
  "ds2.3.1:34.7_0": "ime dhammā bhāvanāya pahātabbahetukā.",
  "ds2.3.1:35.1_0": "Katame dhammā neva dassanena na bhāvanāya pahātabbahetukā?",
  "ds2.3.1:35.2_0": "Te dhamme ṭhapetvā avasesā kusalākusalābyākatā dhammā kāmāvacarā rūpāvacarā,",
  "ds2.3.1:35.2_1": "arūpāvacarā,",
  "ds2.3.1:35.2_2": "apariyāpannā;",
  "ds2.3.1:35.3_0": "vedanākkhandho, saññākkhandho, saṅkhārakkhandho, viññāṇakkhandho;",
  "ds2.3.1:35.4_0": "sabbañca rūpaṁ, asaṅkhatā ca dhātu—",
  "ds2.3.1:35.5_0": "ime dhammā neva dassanena na bhāvanāya pahātabbahetukā.",
  "ds2.3.1:36.0_0": "2.3.1.10. Ācayagāmittika",
  "ds2.3.1:36.1_0": "Katame dhammā ācayagāmino?",
  "ds2.3.1:36.2_0": "Sāsavā kusalākusalā dhammā kāmāvacarā, rūpāvacarā, arūpāvacarā;",
  "ds2.3.1:36.3_0": "vedanākkhandho …pe… viññāṇakkhandho—",
  "ds2.3.1:36.4_0": "ime dhammā ācayagāmino.",
  "ds2.3.1:37.1_0": "Katame dhammā apacayagāmino?",
  "ds2.3.1:37.2_0": "Cattāro maggā apariyāpannā—",
  "ds2.3.1:37.3_0": "ime dhammā apacayagāmino.",
  "ds2.3.1:38.1_0": "Katame dhammā neva ācayagāmi na apacayagāmino?",
  "ds2.3.1:38.2_0": "Kusalākusalānaṁ dhammānaṁ vipākā kāmāvacarā, rūpāvacarā, arūpāvacarā, apariyāpannā;",
  "ds2.3.1:38.3_0": "vedanākkhandho …pe… viññāṇakkhandho;",
  "ds2.3.1:38.4_0": "ye ca dhammā kiriyā neva kusalā nākusalā na ca kammavipākā;",
  "ds2.3.1:38.5_0": "sabbañca rūpaṁ, asaṅkhatā ca dhātu—",
  "ds2.3.1:38.6_0": "ime dhammā neva ācayagāmi na apacayagāmino.",
  "ds2.3.1:39.0_0": "2.3.1.11. Sekkhattika",
  "ds2.3.1:39.1_0": "Katame dhammā sekkhā?",
  "ds2.3.1:39.2_0": "Cattāro maggā apariyāpannā, heṭṭhimāni ca tīṇi sāmaññaphalāni—",
  "ds2.3.1:39.3_0": "ime dhammā sekkhā.",
  "ds2.3.1:40.1_0": "Katame dhammā asekkhā?",
  "ds2.3.1:40.2_0": "Upariṭṭhimaṁ arahattaphalaṁ—",
  "ds2.3.1:40.3_0": "ime dhammā asekkhā.",
  "ds2.3.1:41.1_0": "Katame dhammā nevasekkhanāsekkhā?",
  "ds2.3.1:41.2_0": "Te dhamme ṭhapetvā, avasesā kusalākusalābyākatā dhammā kāmāvacarā, rūpāvacarā, arūpāvacarā;",
  "ds2.3.1:41.3_0": "vedanākkhandho …pe… viññāṇakkhandho;",
  "ds2.3.1:41.4_0": "sabbañca rūpaṁ, asaṅkhatā ca dhātu—",
  "ds2.3.1:41.5_0": "ime dhammā nevasekkhanāsekkhā.",
  "ds2.3.1:42.0_0": "2.3.1.12. Parittattika",
  "ds2.3.1:42.1_0": "Katame dhammā parittā?",
  "ds2.3.1:42.2_0": "Sabbeva kāmāvacarā kusalākusalābyākatā dhammā;",
  "ds2.3.1:42.3_0": "rūpakkhandho …pe… viññāṇakkhandho—",
  "ds2.3.1:42.4_0": "ime dhammā parittā.",
  "ds2.3.1:43.1_0": "Katame dhammā mahaggatā?",
  "ds2.3.1:43.2_0": "Rūpāvacarā, arūpāvacarā, kusalābyākatā dhammā;",
  "ds2.3.1:43.3_0": "vedanākkhandho …pe… viññāṇakkhandho—",
  "ds2.3.1:43.4_0": "ime dhammā mahaggatā.",
  "ds2.3.1:44.1_0": "Katame dhammā appamāṇā?",
  "ds2.3.1:44.2_0": "Apariyāpannā maggā ca, maggaphalāni ca, asaṅkhatā ca dhātu—",
  "ds2.3.1:44.3_0": "ime dhammā appamāṇā.",
  "ds2.3.1:45.0_0": "2.3.1.13. Parittārammaṇattika",
  "ds2.3.1:45.1_0": "Katame dhammā parittārammaṇā?",
  "ds2.3.1:45.2_0": "Paritte dhamme ārabbha ye uppajjanti cittacetasikā dhammā—",
  "ds2.3.1:45.3_0": "ime dhammā parittārammaṇā.",
  "ds2.3.1:46.1_0": "Katame dhammā mahaggatārammaṇā?",
  "ds2.3.1:46.2_0": "Mahaggate dhamme ārabbha ye uppajjanti cittacetasikā dhammā—",
  "ds2.3.1:46.3_0": "ime dhammā mahaggatārammaṇā.",
  "ds2.3.1:47.1_0": "Katame dhammā appamāṇārammaṇā?",
  "ds2.3.1:47.2_0": "Appamāṇe dhamme ārabbha ye uppajjanti cittacetasikā dhammā—",
  "ds2.3.1:47.3_0": "ime dhammā appamāṇārammaṇā.",
  "ds2.3.1:48.0_0": "2.3.1.14. Hīnattika",
  "ds2.3.1:48.1_0": "Katame dhammā hīnā?",
  "ds2.3.1:48.2_0": "Tīṇi akusalamūlāni—",
  "ds2.3.1:48.3_0": "lobho, doso, moho;",
  "ds2.3.1:48.4_0": "tadekaṭṭhā ca kilesā;",
  "ds2.3.1:48.5_0": "taṁsampayutto vedanākkhandho …pe… viññāṇakkhandho;",
  "ds2.3.1:48.6_0": "taṁsamuṭṭhānaṁ kāyakammaṁ, vacīkammaṁ, manokammaṁ—",
  "ds2.3.1:48.7_0": "ime dhammā hīnā.",
  "ds2.3.1:49.1_0": "Katame dhammā majjhimā?",
  "ds2.3.1:49.2_0": "Sāsavā kusalābyākatā dhammā kāmāvacarā, rūpāvacarā, arūpāvacarā, rūpakkhandho …pe… viññāṇakkhandho—",
  "ds2.3.1:49.3_0": "ime dhammā majjhimā.",
  "ds2.3.1:50.1_0": "Katame dhammā paṇītā?",
  "ds2.3.1:50.2_0": "Apariyāpannā maggā ca, maggaphalāni ca, asaṅkhatā ca dhātu—",
  "ds2.3.1:50.3_0": "ime dhammā paṇītā.",
  "ds2.3.1:51.0_0": "2.3.1.15. Micchattaniyatattika",
  "ds2.3.1:51.1_0": "Katame dhammā micchattaniyatā?",
  "ds2.3.1:51.2_0": "Pañca kammāni ānantarikāni, yā ca micchādiṭṭhiniyatā—",
  "ds2.3.1:51.3_0": "ime dhammā micchattaniyatā.",
  "ds2.3.1:52.1_0": "Katame dhammā sammattaniyatā?",
  "ds2.3.1:52.2_0": "Cattāro maggā apariyāpannā—",
  "ds2.3.1:52.3_0": "ime dhammā sammattaniyatā.",
  "ds2.3.1:53.1_0": "Katame dhammā aniyatā?",
  "ds2.3.1:53.2_0": "Te dhamme ṭhapetvā,",
  "ds2.3.1:53.2_1": "avasesā kusalākusalābyākatā dhammā kāmāvacarā,",
  "ds2.3.1:53.2_2": "rūpāvacarā,",
  "ds2.3.1:53.2_3": "arūpāvacarā,",
  "ds2.3.1:53.2_4": "apariyāpannā;",
  "ds2.3.1:53.3_0": "vedanākkhandho …pe… viññāṇakkhandho;",
  "ds2.3.1:53.4_0": "sabbañca rūpaṁ, asaṅkhatā ca dhātu—",
  "ds2.3.1:53.5_0": "ime dhammā aniyatā.",
  "ds2.3.1:54.0_0": "2.3.1.16. Maggārammaṇattika",
  "ds2.3.1:54.1_0": "Katame dhammā maggārammaṇā?",
  "ds2.3.1:54.2_0": "Ariyamaggaṁ ārabbha ye uppajjanti cittacetasikā dhammā—",
  "ds2.3.1:54.3_0": "ime dhammā maggārammaṇā.",
  "ds2.3.1:55.1_0": "Katame dhammā maggahetukā?",
  "ds2.3.1:55.2_0": "Ariyamaggasamaṅgissa maggaṅgāni ṭhapetvā;",
  "ds2.3.1:55.3_0": "taṁsampayutto vedanākkhandho …pe… viññāṇakkhandho—",
  "ds2.3.1:55.4_0": "ime dhammā maggahetukā.",
  "ds2.3.1:55.5_0": "Ariyamaggasamaṅgissa sammādiṭṭhi maggo ceva hetu ca,",
  "ds2.3.1:55.5_1": "sammādiṭṭhiṁ ṭhapetvā,",
  "ds2.3.1:55.5_2": "taṁsampayutto vedanākkhandho …pe…",
  "ds2.3.1:55.5_3": "viññāṇakkhandho—",
  "ds2.3.1:55.6_0": "ime dhammā maggahetukā.",
  "ds2.3.1:55.7_0": "Ariyamaggasamaṅgissa alobho, adoso, amoho—",
  "ds2.3.1:55.8_0": "ime dhammā maggahetū.",
  "ds2.3.1:55.9_0": "Taṁsampayutto vedanākkhandho …pe… viññāṇakkhandho—",
  "ds2.3.1:55.10_0": "ime dhammā maggahetukā.",
  "ds2.3.1:56.1_0": "Katame dhammā maggādhipatino?",
  "ds2.3.1:56.2_0": "Ariyamaggaṁ adhipatiṁ karitvā ye uppajjanti cittacetasikā dhammā—",
  "ds2.3.1:56.3_0": "ime dhammā maggādhipatino.",
  "ds2.3.1:56.4_0": "Ariyamaggasamaṅgissa vīmaṁsādhipateyyaṁ maggaṁ bhāvayantassa vīmaṁsaṁ ṭhapetvā;",
  "ds2.3.1:56.5_0": "taṁsampayutto vedanākkhandho …pe… viññāṇakkhandho—",
  "ds2.3.1:56.6_0": "ime dhammā maggādhipatino.",
  "ds2.3.1:57.0_0": "2.3.1.17. Uppannattika",
  "ds2.3.1:57.1_0": "Katame dhammā uppannā?",
  "ds2.3.1:57.2_0": "Ye dhammā jātā bhūtā sañjātā nibbattā abhinibbattā pātubhūtā uppannā samuppannā uṭṭhitā samuṭṭhitā uppannā uppannaṁsena saṅgahitā, rūpaṁ, vedanā, saññā, saṅkhārā, viññāṇaṁ—",
  "ds2.3.1:57.3_0": "ime dhammā uppannā.",
  "ds2.3.1:58.1_0": "Katame dhammā anuppannā?",
  "ds2.3.1:58.2_0": "Ye dhammā ajātā abhūtā asañjātā anibbattā anabhinibbattā apātubhūtā anuppannā asamuppannā anuṭṭhitā asamuṭṭhitā anuppannā anuppannaṁsena saṅgahitā, rūpaṁ, vedanā, saññā, saṅkhārā, viññāṇaṁ—",
  "ds2.3.1:58.3_0": "ime dhammā anuppannā.",
  "ds2.3.1:59.1_0": "Katame dhammā uppādino?",
  "ds2.3.1:59.2_0": "Kusalākusalānaṁ dhammānaṁ avipakkavipākānaṁ vipākā kāmāvacarā,",
  "ds2.3.1:59.2_1": "rūpāvacarā,",
  "ds2.3.1:59.2_2": "arūpāvacarā,",
  "ds2.3.1:59.2_3": "apariyāpannā;",
  "ds2.3.1:59.3_0": "vedanākkhandho …pe… viññāṇakkhandho;",
  "ds2.3.1:59.4_0": "yañca rūpaṁ kammassa katattā uppajjissati—",
  "ds2.3.1:59.5_0": "ime dhammā uppādino.",
  "ds2.3.1:60.0_0": "2.3.1.18. Atītattika",
  "ds2.3.1:60.1_0": "Katame dhammā atītā?",
  "ds2.3.1:60.2_0": "Ye dhammā atītā niruddhā vigatā vipariṇatā atthaṅgatā abbhatthaṅgatā uppajjitvā vigatā atītā atītaṁsena saṅgahitā, rūpaṁ, vedanā, saññā, saṅkhārā, viññāṇaṁ—",
  "ds2.3.1:60.3_0": "ime dhammā atītā.",
  "ds2.3.1:61.1_0": "Katame dhammā anāgatā?",
  "ds2.3.1:61.2_0": "Ye dhammā ajātā abhūtā asañjātā anibbattā anabhinibbattā apātubhūtā anuppannā asamuppannā anuṭṭhitā asamuṭṭhitā anāgatā anāgataṁsena saṅgahitā, rūpaṁ, vedanā, saññā, saṅkhārā, viññāṇaṁ—",
  "ds2.3.1:61.3_0": "ime dhammā anāgatā.",
  "ds2.3.1:62.1_0": "Katame dhammā paccuppannā?",
  "ds2.3.1:62.2_0": "Ye dhammā jātā bhūtā sañjātā nibbattā abhinibbattā pātubhūtā uppannā samuppannā uṭṭhitā samuṭṭhitā paccuppannā paccuppannaṁsena saṅgahitā, rūpaṁ, vedanā, saññā, saṅkhārā, viññāṇaṁ—",
  "ds2.3.1:62.3_0": "ime dhammā paccuppannā.",
  "ds2.3.1:63.0_0": "2.3.1.19. Atītārammaṇattika",
  "ds2.3.1:63.1_0": "Katame dhammā atītārammaṇā?",
  "ds2.3.1:63.2_0": "Atīte dhamme ārabbha ye uppajjanti cittacetasikā dhammā—",
  "ds2.3.1:63.3_0": "ime dhammā atītārammaṇā.",
  "ds2.3.1:64.1_0": "Katame dhammā anāgatārammaṇā?",
  "ds2.3.1:64.2_0": "Anāgate dhamme ārabbha ye uppajjanti cittacetasikā dhammā—",
  "ds2.3.1:64.3_0": "ime dhammā anāgatārammaṇā.",
  "ds2.3.1:65.1_0": "Katame dhammā paccuppannārammaṇā?",
  "ds2.3.1:65.2_0": "Paccuppanne dhamme ārabbha ye uppajjanti cittacetasikā dhammā—",
  "ds2.3.1:65.3_0": "ime dhammā paccuppannārammaṇā.",
  "ds2.3.1:66.0_0": "2.3.1.20. Ajjhattattika",
  "ds2.3.1:66.1_0": "Katame dhammā ajjhattā?",
  "ds2.3.1:66.2_0": "Ye dhammā tesaṁ tesaṁ sattānaṁ ajjhattaṁ paccattaṁ niyatā pāṭipuggalikā upādinnā, rūpaṁ, vedanā, saññā, saṅkhārā, viññāṇaṁ—",
  "ds2.3.1:66.3_0": "ime dhammā ajjhattā.",
  "ds2.3.1:67.1_0": "Katame dhammā bahiddhā?",
  "ds2.3.1:67.2_0": "Ye dhammā tesaṁ tesaṁ parasattānaṁ parapuggalānaṁ ajjhattaṁ paccattaṁ niyatā pāṭipuggalikā upādinnā, rūpaṁ, vedanā, saññā, saṅkhārā, viññāṇaṁ—",
  "ds2.3.1:67.3_0": "ime dhammā bahiddhā.",
  "ds2.3.1:68.1_0": "Katame dhammā ajjhattabahiddhā?",
  "ds2.3.1:68.2_0": "Tadubhayaṁ—",
  "ds2.3.1:68.3_0": "ime dhammā ajjhattabahiddhā.",
  "ds2.3.1:69.0_0": "2.3.1.21. Ajjhattārammaṇattika",
  "ds2.3.1:69.1_0": "Katame dhammā ajjhattārammaṇā?",
  "ds2.3.1:69.2_0": "Ajjhatte dhamme ārabbha ye uppajjanti cittacetasikā dhammā—",
  "ds2.3.1:69.3_0": "ime dhammā ajjhattārammaṇā.",
  "ds2.3.1:70.1_0": "Katame dhammā bahiddhārammaṇā?",
  "ds2.3.1:70.2_0": "Bahiddhā dhamme ārabbha ye uppajjanti cittacetasikā dhammā—",
  "ds2.3.1:70.3_0": "ime dhammā bahiddhārammaṇā.",
  "ds2.3.1:71.1_0": "Katame dhammā ajjhattabahiddhārammaṇā?",
  "ds2.3.1:71.2_0": "Ajjhattabahiddhā dhamme ārabbha ye uppajjanti cittacetasikā dhammā—",
  "ds2.3.1:71.3_0": "ime dhammā ajjhattabahiddhārammaṇā.",
  "ds2.3.1:72.0_0": "2.3.1.22. Sanidassanattika",
  "ds2.3.1:72.1_0": "Katame dhammā sanidassanasappaṭighā?",
  "ds2.3.1:72.2_0": "Rūpāyatanaṁ—",
  "ds2.3.1:72.3_0": "ime dhammā sanidassanasappaṭighā.",
  "ds2.3.1:73.1_0": "Katame dhammā anidassanasappaṭighā?",
  "ds2.3.1:73.2_0": "Cakkhāyatanaṁ,",
  "ds2.3.1:73.2_1": "sotāyatanaṁ,",
  "ds2.3.1:73.2_2": "ghānāyatanaṁ,",
  "ds2.3.1:73.2_3": "jivhāyatanaṁ,",
  "ds2.3.1:73.2_4": "kāyāyatanaṁ,",
  "ds2.3.1:73.2_5": "saddāyatanaṁ,",
  "ds2.3.1:73.2_6": "gandhāyatanaṁ,",
  "ds2.3.1:73.2_7": "rasāyatanaṁ,",
  "ds2.3.1:73.2_8": "phoṭṭhabbāyatanaṁ—",
  "ds2.3.1:73.3_0": "ime dhammā anidassanasappaṭighā.",
  "ds2.3.1:74.1_0": "Katame dhammā anidassanaappaṭighā?",
  "ds2.3.1:74.2_0": "Vedanākkhandho, saññākkhandho, saṅkhārakkhandho, viññāṇakkhandho;",
  "ds2.3.1:74.3_0": "yañca rūpaṁ anidassanaṁ appaṭighaṁ dhammāyatanapariyāpannaṁ;",
  "ds2.3.1:74.4_0": "asaṅkhatā ca dhātu—",
  "ds2.3.1:74.5_0": "ime dhammā anidassanaappaṭighā.",
  "ds2.3.1:74.6_0": "Tikaṁ."
}