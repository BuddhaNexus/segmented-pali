{
  "tha-ap168:0.1_0": "Therāpadāna",
  "tha-ap168:0.2_0": "Supāricariyavagga",
  "tha-ap168:0.3_0": "6. Sappidāyakattheraapadāna",
  "tha-ap168:1.1_0": "“Phusso nāmāsi bhagavā,",
  "tha-ap168:1.2_0": "āhutīnaṁ paṭiggaho;",
  "tha-ap168:1.3_0": "Gacchate vīthiyaṁ vīro,",
  "tha-ap168:1.4_0": "nibbāpento mahājanaṁ.",
  "tha-ap168:2.1_0": "Anupubbena bhagavā,",
  "tha-ap168:2.2_0": "āgacchi mama santikaṁ;",
  "tha-ap168:2.3_0": "Tato taṁ pattaṁ paggayha,",
  "tha-ap168:2.4_0": "sappitelamadāsahaṁ.",
  "tha-ap168:3.1_0": "Dvenavute ito kappe,",
  "tha-ap168:3.2_0": "yaṁ sappimadadiṁ tadā;",
  "tha-ap168:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap168:3.4_0": "sappidānassidaṁ phalaṁ.",
  "tha-ap168:4.1_0": "Chappaññāse ito kappe,",
  "tha-ap168:4.2_0": "eko āsi samodako;",
  "tha-ap168:4.3_0": "Sattaratanasampanno,",
  "tha-ap168:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap168:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap168:5.2_0": "…pe…",
  "tha-ap168:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap168:6.1_0": "Itthaṁ sudaṁ āyasmā sappidāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap168:7.1_0": "Sappidāyakattherassāpadānaṁ chaṭṭhaṁ."
}