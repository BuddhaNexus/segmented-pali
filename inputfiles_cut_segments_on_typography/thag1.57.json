{
  "thag1.57:0.1_0": "Theragāthā 1.57",
  "thag1.57:0.2_0": "Ekakanipāta",
  "thag1.57:0.3_0": "Chaṭṭhavagga",
  "thag1.57:0.4_0": "Dutiyakuṭivihārittheragāthā",
  "thag1.57:1.1_0": "“Ayamāhu purāṇiyā kuṭi,",
  "thag1.57:1.2_0": "Aññaṁ patthayase navaṁ kuṭiṁ;",
  "thag1.57:1.3_0": "Āsaṁ kuṭiyā virājaya,",
  "thag1.57:1.4_0": "Dukkhā bhikkhu puna navā kuṭī”ti.",
  "thag1.57:1.5_0": "… Dutiyakuṭivihāritthero …."
}