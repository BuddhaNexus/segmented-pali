{
  "patthana15.87:0.1_0": "Paṭṭhānapakaraṇa",
  "patthana15.87:0.2_0": "Dhammānulomapaccanīya (3), Dukatikapaṭṭhānapāḷi (3)",
  "patthana15.87:0.3_0": "15.87. Saraṇaduka, Atītattika",
  "patthana15.87:1.1_0": "Saraṇo atīto dhammo nasaraṇassa naatītassa dhammassa ārammaṇapaccayena paccayo.",
  "patthana15.87:1.1_1": "Ārammaṇe cattāri,",
  "patthana15.87:1.1_2": "adhipatiyā tīṇi,",
  "patthana15.87:1.1_3": "upanissaye cattāri.",
  "patthana15.87:1.1_4": "(Anāgato atītasadiso.)"
}