{
  "thag4.3:0.1_0": "Theragāthā 4.3",
  "thag4.3:0.2_0": "Catukkanipāta",
  "thag4.3:0.3_0": "Paṭhamavagga",
  "thag4.3:0.4_0": "Sabhiyattheragāthā",
  "thag4.3:1.1_0": "“Pare ca na vijānanti,",
  "thag4.3:1.2_0": "mayamettha yamāmase;",
  "thag4.3:1.3_0": "Ye ca tattha vijānanti,",
  "thag4.3:1.4_0": "tato sammanti medhagā.",
  "thag4.3:2.1_0": "Yadā ca avijānantā,",
  "thag4.3:2.2_0": "iriyantyamarā viya;",
  "thag4.3:2.3_0": "Vijānanti ca ye dhammaṁ,",
  "thag4.3:2.4_0": "āturesu anāturā.",
  "thag4.3:3.1_0": "Yaṁ kiñci sithilaṁ kammaṁ,",
  "thag4.3:3.2_0": "saṅkiliṭṭhañca yaṁ vataṁ;",
  "thag4.3:3.3_0": "Saṅkassaraṁ brahmacariyaṁ,",
  "thag4.3:3.4_0": "na taṁ hoti mahapphalaṁ.",
  "thag4.3:4.1_0": "Yassa sabrahmacārīsu,",
  "thag4.3:4.2_0": "gāravo nūpalabbhati;",
  "thag4.3:4.3_0": "Ārakā hoti saddhammā,",
  "thag4.3:4.4_0": "nabhaṁ puthaviyā yathā”ti.",
  "thag4.3:4.5_0": "… Sabhiyo thero …."
}