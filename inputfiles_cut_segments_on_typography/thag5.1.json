{
  "thag5.1:0.1_0": "Theragāthā 5.1",
  "thag5.1:0.2_0": "Pañcakanipāta",
  "thag5.1:0.3_0": "Paṭhamavagga",
  "thag5.1:0.4_0": "Rājadattattheragāthā",
  "thag5.1:1.1_0": "“Bhikkhu sivathikaṁ gantvā,",
  "thag5.1:1.2_0": "Addasa itthimujjhitaṁ;",
  "thag5.1:1.3_0": "Apaviddhaṁ susānasmiṁ,",
  "thag5.1:1.4_0": "Khajjantiṁ kimihī phuṭaṁ.",
  "thag5.1:2.1_0": "Yañhi eke jigucchanti,",
  "thag5.1:2.2_0": "mataṁ disvāna pāpakaṁ;",
  "thag5.1:2.3_0": "Kāmarāgo pāturahu,",
  "thag5.1:2.4_0": "andhova savatī ahuṁ.",
  "thag5.1:3.1_0": "Oraṁ odanapākamhā,",
  "thag5.1:3.2_0": "tamhā ṭhānā apakkamiṁ;",
  "thag5.1:3.3_0": "Satimā sampajānohaṁ,",
  "thag5.1:3.4_0": "ekamantaṁ upāvisiṁ.",
  "thag5.1:4.1_0": "Tato me manasīkāro,",
  "thag5.1:4.2_0": "yoniso udapajjatha;",
  "thag5.1:4.3_0": "Ādīnavo pāturahu,",
  "thag5.1:4.4_0": "nibbidā samatiṭṭhatha.",
  "thag5.1:5.1_0": "Tato cittaṁ vimucci me,",
  "thag5.1:5.2_0": "passa dhammasudhammataṁ;",
  "thag5.1:5.3_0": "Tisso vijjā anuppattā,",
  "thag5.1:5.4_0": "kataṁ buddhassa sāsanan”ti.",
  "thag5.1:5.5_0": "… Rājadatto thero …."
}