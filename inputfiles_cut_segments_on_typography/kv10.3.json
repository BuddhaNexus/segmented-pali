{
  "kv10.3:0.1_0": "Kathāvatthu",
  "kv10.3:0.2_0": "Mahāpaṇṇāsaka",
  "kv10.3:0.3_0": "Dasamavagga",
  "kv10.3:0.4_0": "Pañcaviññāṇasamaṅgissamaggakathā",
  "kv10.3:1.1_0": "Pañcaviññāṇasamaṅgissa atthi maggabhāvanāti?",
  "kv10.3:1.2_0": "Āmantā.",
  "kv10.3:1.3_0": "Nanu pañcaviññāṇā uppannavatthukā uppannārammaṇāti?",
  "kv10.3:1.4_0": "Āmantā.",
  "kv10.3:1.5_0": "Hañci pañcaviññāṇā uppannavatthukā uppannārammaṇā, no ca vata re vattabbe—",
  "kv10.3:1.6_0": "“pañcaviññāṇasamaṅgissa atthi maggabhāvanā”ti.",
  "kv10.3:2.1_0": "Nanu pañcaviññāṇā purejātavatthukā purejātārammaṇā ajjhattikavatthukā bāhirārammaṇā asambhinnavatthukā",
  "kv10.3:2.1_1": "asambhinnārammaṇā nānāvatthukā nānārammaṇā na aññamaññassa gocaravisayaṁ paccanubhonti,",
  "kv10.3:2.1_2": "na asamannāhārā uppajjanti,",
  "kv10.3:2.1_3": "na amanasikārā uppajjanti,",
  "kv10.3:2.1_4": "na abbokiṇṇā uppajjanti,",
  "kv10.3:2.1_5": "na apubbaṁ acarimaṁ uppajjanti,",
  "kv10.3:2.1_6": "na aññamaññassa samanantarā uppajjanti,",
  "kv10.3:2.1_7": "nanu pañcaviññāṇā anābhogāti?",
  "kv10.3:2.2_0": "Āmantā.",
  "kv10.3:2.3_0": "Hañci pañcaviññāṇā anābhogā, no ca vata re vattabbe—",
  "kv10.3:2.4_0": "“pañcaviññāṇasamaṅgissa atthi maggabhāvanā”ti.",
  "kv10.3:3.1_0": "Cakkhuviññāṇasamaṅgissa atthi maggabhāvanāti?",
  "kv10.3:3.2_0": "Āmantā.",
  "kv10.3:3.3_0": "Cakkhuviññāṇaṁ suññataṁ ārabbha uppajjatīti?",
  "kv10.3:3.4_0": "Na hevaṁ vattabbe …pe…",
  "kv10.3:3.5_0": "cakkhuviññāṇaṁ suññataṁ ārabbha uppajjatīti?",
  "kv10.3:3.6_0": "Āmantā.",
  "kv10.3:3.7_0": "Cakkhuñca paṭicca suññatañca uppajjati cakkhuviññāṇanti?",
  "kv10.3:3.8_0": "Na hevaṁ vattabbe …pe…",
  "kv10.3:3.9_0": "cakkhuñca paṭicca suññatañca uppajjati cakkhuviññāṇanti?",
  "kv10.3:3.10_0": "Āmantā.",
  "kv10.3:3.11_0": "“Cakkhuñca paṭicca suññatañca uppajjati cakkhuviññāṇan”ti—",
  "kv10.3:3.12_0": "attheva suttantoti?",
  "kv10.3:3.13_0": "Natthi.",
  "kv10.3:3.14_0": "“Cakkhuñca paṭicca rūpe ca uppajjati cakkhuviññāṇan”ti—",
  "kv10.3:3.15_0": "attheva suttantoti?",
  "kv10.3:3.16_0": "Āmantā.",
  "kv10.3:3.17_0": "Hañci “cakkhuñca paṭicca rūpe ca uppajjati cakkhuviññāṇan”ti—",
  "kv10.3:3.18_0": "attheva suttanto, no ca vata re vattabbe—",
  "kv10.3:3.19_0": "“cakkhuñca paṭicca suññatañca uppajjati cakkhuviññāṇan”ti.",
  "kv10.3:4.1_0": "Cakkhuviññāṇasamaṅgissa atthi maggabhāvanāti?",
  "kv10.3:4.2_0": "Āmantā.",
  "kv10.3:4.3_0": "Cakkhuviññāṇaṁ atītānāgataṁ ārabbha uppajjatīti?",
  "kv10.3:4.4_0": "Na hevaṁ vattabbe …pe…",
  "kv10.3:4.5_0": "cakkhuviññāṇasamaṅgissa atthi maggabhāvanāti?",
  "kv10.3:4.6_0": "Āmantā.",
  "kv10.3:4.7_0": "Cakkhuviññāṇaṁ phassaṁ ārabbha …",
  "kv10.3:4.8_0": "vedanaṁ ārabbha …",
  "kv10.3:4.9_0": "saññaṁ ārabbha …",
  "kv10.3:4.10_0": "cetanaṁ ārabbha …",
  "kv10.3:4.11_0": "cittaṁ ārabbha …",
  "kv10.3:4.12_0": "cakkhuṁ ārabbha …pe…",
  "kv10.3:4.13_0": "kāyaṁ ārabbha …pe…",
  "kv10.3:4.14_0": "saddaṁ ārabbha …pe…",
  "kv10.3:4.15_0": "phoṭṭhabbaṁ ārabbha uppajjatīti?",
  "kv10.3:4.16_0": "Na hevaṁ vattabbe …pe….",
  "kv10.3:5.1_0": "Manoviññāṇasamaṅgissa atthi maggabhāvanā, manoviññāṇaṁ suññataṁ ārabbha uppajjatīti?",
  "kv10.3:5.2_0": "Āmantā.",
  "kv10.3:5.3_0": "Cakkhuviññāṇasamaṅgissa atthi maggabhāvanā, cakkhuviññāṇaṁ suññataṁ ārabbha uppajjatīti?",
  "kv10.3:5.4_0": "Na hevaṁ vattabbe …pe…",
  "kv10.3:5.5_0": "manoviññāṇasamaṅgissa atthi maggabhāvanā, manoviññāṇaṁ atītānāgataṁ ārabbha uppajjatīti?",
  "kv10.3:5.6_0": "Āmantā.",
  "kv10.3:5.7_0": "Cakkhuviññāṇasamaṅgissa atthi maggabhāvanā, cakkhuviññāṇaṁ atītānāgataṁ ārabbha uppajjatīti?",
  "kv10.3:5.8_0": "Na hevaṁ vattabbe …pe…",
  "kv10.3:5.9_0": "manoviññāṇasamaṅgissa atthi maggabhāvanā, manoviññāṇaṁ phassaṁ ārabbha …",
  "kv10.3:5.10_0": "vedanaṁ ārabbha …pe…",
  "kv10.3:5.11_0": "phoṭṭhabbaṁ ārabbha uppajjatīti?",
  "kv10.3:5.12_0": "Āmantā.",
  "kv10.3:5.13_0": "Cakkhuviññāṇasamaṅgissa atthi maggabhāvanā, cakkhuviññāṇaṁ phassaṁ ārabbha …",
  "kv10.3:5.14_0": "vedanaṁ ārabbha …pe…",
  "kv10.3:5.15_0": "phoṭṭhabbaṁ ārabbha uppajjatīti?",
  "kv10.3:5.16_0": "Na hevaṁ vattabbe …pe….",
  "kv10.3:6.1_0": "Na vattabbaṁ—",
  "kv10.3:6.2_0": "“pañcaviññāṇasamaṅgissa atthi maggabhāvanā”ti?",
  "kv10.3:6.3_0": "Āmantā.",
  "kv10.3:6.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv10.3:6.5_0": "“idha, bhikkhave, bhikkhu cakkhunā rūpaṁ disvā na nimittaggāhī hoti nānubyañjanaggāhī …pe…",
  "kv10.3:6.6_0": "sotena saddaṁ sutvā …pe…",
  "kv10.3:6.7_0": "ghānena gandhaṁ ghāyitvā …pe…",
  "kv10.3:6.8_0": "jivhāya rasaṁ sāyitvā …pe…",
  "kv10.3:6.9_0": "kāyena phoṭṭhabbaṁ phusitvā na nimittaggāhī hoti nānubyañjanaggāhī”ti.",
  "kv10.3:6.10_0": "Attheva suttantoti?",
  "kv10.3:6.11_0": "Āmantā.",
  "kv10.3:6.12_0": "Tena hi pañcaviññāṇasamaṅgissa atthi maggabhāvanāti.",
  "kv10.3:6.13_0": "Pañcaviññāṇasamaṅgissa maggakathā niṭṭhitā."
}