{
  "thag1.43:0.1_0": "Theragāthā 1.43",
  "thag1.43:0.2_0": "Ekakanipāta",
  "thag1.43:0.3_0": "Pañcamavagga",
  "thag1.43:0.4_0": "Sumaṅgalattheragāthā",
  "thag1.43:1.1_0": "“Sumuttiko sumuttiko,",
  "thag1.43:1.2_0": "Sāhu sumuttikomhi tīhi khujjakehi;",
  "thag1.43:1.3_0": "Asitāsu mayā naṅgalāsu mayā,",
  "thag1.43:1.4_0": "Khuddakuddālāsu mayā.",
  "thag1.43:1.5_0": "Yadipi idhameva idhameva,",
  "thag1.43:1.6_0": "Atha vāpi alameva alameva;",
  "thag1.43:1.7_0": "Jhāya sumaṅgala jhāya sumaṅgala,",
  "thag1.43:1.8_0": "Appamatto vihara sumaṅgalā”ti.",
  "thag1.43:1.9_0": "… Sumaṅgalo thero …."
}