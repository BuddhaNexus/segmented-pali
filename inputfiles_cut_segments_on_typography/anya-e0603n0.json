{
  "anya-e0603n0:0_0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e0603n0:1_0": "Lakkhaṇāto",
  "anya-e0603n0:2_0": "Buddhathomanāgāthā",
  "anya-e0603n0:3_0": "1.",
  "anya-e0603n0:4_0": "Bhavābhavesu nekesu,",
  "anya-e0603n0:5_0": "Pūresi tiṃsapāramī;",
  "anya-e0603n0:6_0": "Cariyāyoca sambuddho,",
  "anya-e0603n0:7_0": "Pariccāgejahaṃsadā.",
  "anya-e0603n0:8_0": "2.",
  "anya-e0603n0:9_0": "Vessantarattabhāvamhi,",
  "anya-e0603n0:10_0": "Ṭhitomaddiṃ pisoadā;",
  "anya-e0603n0:11_0": "Nijaṃkaṇhājinaṃjāliṃ,",
  "anya-e0603n0:12_0": "Viyaṃvaṅkatapabbate.",
  "anya-e0603n0:13_0": "3.",
  "anya-e0603n0:14_0": "Tatocavitvātusite,",
  "anya-e0603n0:15_0": "Pureuppajjipuññavā;",
  "anya-e0603n0:16_0": "Rūpādidasaṭhānehi,",
  "anya-e0603n0:17_0": "Aññedeveatikkami.",
  "anya-e0603n0:18_0": "4.",
  "anya-e0603n0:19_0": "Cakkavāḷasahassehi,",
  "anya-e0603n0:20_0": "Dasahāgammasabbaso;",
  "anya-e0603n0:21_0": "Yācitodevabrahmūhi,",
  "anya-e0603n0:22_0": "Buddhabhāvamanonato.",
  "anya-e0603n0:23_0": "5.",
  "anya-e0603n0:24_0": "Vilokanānisopañca,",
  "anya-e0603n0:25_0": "Viloketvāsirindharo;",
  "anya-e0603n0:26_0": "Tatocavitvāmāyāya,",
  "anya-e0603n0:27_0": "Uppajjisākiyekule.",
  "anya-e0603n0:28_0": "6.",
  "anya-e0603n0:29_0": "Visākhapuṇṇameseṭṭhe,",
  "anya-e0603n0:30_0": "Vijāyilumbinīvane;",
  "anya-e0603n0:31_0": "Sīhanādaṃpanādento,",
  "anya-e0603n0:32_0": "Sattapādaṃgatotadā.",
  "anya-e0603n0:33_0": "8.",
  "anya-e0603n0:34_0": "Virociṃsutadākāse,",
  "anya-e0603n0:35_0": "Tārocandadivākarā;",
  "anya-e0603n0:36_0": "Ratanānicabhūmaṭṭhā,",
  "anya-e0603n0:37_0": "Nānāpupphācapupphare.",
  "anya-e0603n0:38_0": "8.",
  "anya-e0603n0:39_0": "Sītāyanti avīcaggī,",
  "anya-e0603n0:40_0": "Khuppipāsācasohitā;",
  "anya-e0603n0:41_0": "Lokantaremahāpabhā,",
  "anya-e0603n0:42_0": "Andhāpassantitāvade.",
  "anya-e0603n0:43_0": "9.",
  "anya-e0603n0:44_0": "Aghaṭṭitāvāditāni,",
  "anya-e0603n0:45_0": "Tūriyānicaambare;",
  "anya-e0603n0:46_0": "Madhurenapanādiṃsu,",
  "anya-e0603n0:47_0": "Yasaṃsarenatāvade.",
  "anya-e0603n0:48_0": "10.",
  "anya-e0603n0:49_0": "Thutigītānigāyantā,",
  "anya-e0603n0:50_0": "Devācakamalāsanā;",
  "anya-e0603n0:51_0": "Sammodentāvaaṭṭhaṃsu,",
  "anya-e0603n0:52_0": "Buddhaṅkurassasantike.",
  "anya-e0603n0:53_0": "11.",
  "anya-e0603n0:54_0": "Kumārocārudhātīhi,",
  "anya-e0603n0:55_0": "Tositārājamandire;",
  "anya-e0603n0:56_0": "Vuddhosisusukhaṃkhemaṃ,",
  "anya-e0603n0:57_0": "Sakyakaññānamantare.",
  "anya-e0603n0:58_0": "12.",
  "anya-e0603n0:59_0": "Soḷasavassikosakya,",
  "anya-e0603n0:60_0": "Nandanopitarāsatā;",
  "anya-e0603n0:61_0": "Rājatteabhisittosi,",
  "anya-e0603n0:62_0": "Modesituvanattaye.",
  "anya-e0603n0:63_0": "13.",
  "anya-e0603n0:64_0": "Nandanānaṃsovaṇṇa,",
  "anya-e0603n0:65_0": "Vaṇṇasobhaṃyasodharaṃ;",
  "anya-e0603n0:66_0": "Sokāresimahesittaṃ,",
  "anya-e0603n0:67_0": "Lokanettarasāyanaṃ.",
  "anya-e0603n0:68_0": "14.",
  "anya-e0603n0:69_0": "Ekūnatiṃsamevasse,",
  "anya-e0603n0:70_0": "Visākhapuṇṇamevare;",
  "anya-e0603n0:71_0": "Sahasenāyauyyāsi,",
  "anya-e0603n0:72_0": "Yānenuyyānamaṇḍalaṃ.",
  "anya-e0603n0:73_0": "15.",
  "anya-e0603n0:74_0": "Nimittecaturodisvā,",
  "anya-e0603n0:75_0": "Puttassacavijāyanaṃ;",
  "anya-e0603n0:76_0": "Sutvāubbiggacittena,",
  "anya-e0603n0:77_0": "Naramitthapuretahiṃ.",
  "anya-e0603n0:78_0": "16.",
  "anya-e0603n0:79_0": "Paccāgammapuraṃtamhā,",
  "anya-e0603n0:80_0": "Pattopāsādamaṇḍalaṃ;",
  "anya-e0603n0:81_0": "Nippajjidevarājāva,",
  "anya-e0603n0:82_0": "Sayanesomahārahe.",
  "anya-e0603n0:83_0": "17.",
  "anya-e0603n0:84_0": "Sundarītaṃpurakkhitvā,",
  "anya-e0603n0:85_0": "Naccagītenatosayuṃ;",
  "anya-e0603n0:86_0": "Evaṃpisonaramitvā,",
  "anya-e0603n0:87_0": "Āhunikkhamamānaso.",
  "anya-e0603n0:88_0": "18.",
  "anya-e0603n0:89_0": "Pathamaṃrāhulaṃputtaṃ,",
  "anya-e0603n0:90_0": "Passitvānikkhamāmahaṃ;",
  "anya-e0603n0:91_0": "Iticintiyamāneso,",
  "anya-e0603n0:92_0": "Agājāyānivesanaṃ;",
  "anya-e0603n0:93_0": "19.",
  "anya-e0603n0:94_0": "Thapetvāpādadummāre,",
  "anya-e0603n0:95_0": "Gīvamantopavesayaṃ;",
  "anya-e0603n0:96_0": "Olokayaṃsapassittha,",
  "anya-e0603n0:97_0": "Nippannaṃtaṃsamātaraṃ.",
  "anya-e0603n0:98_0": "20.",
  "anya-e0603n0:99_0": "Gaṇheyyaṃtaṃapanetvā,",
  "anya-e0603n0:100_0": "Bāhuṃcedeviyāahaṃ;",
  "anya-e0603n0:101_0": "Nadadenikkhamokāsaṃ,",
  "anya-e0603n0:102_0": "Pabujjhitvāyasodharā.",
  "anya-e0603n0:103_0": "21.",
  "anya-e0603n0:104_0": "Buddhapattovapasseyyaṃ,",
  "anya-e0603n0:105_0": "Rāhulaṃiticintiya;",
  "anya-e0603n0:106_0": "Kaṇḍakaṃāruhitvāna,",
  "anya-e0603n0:107_0": "Tamhāchannenanikkhami.",
  "anya-e0603n0:108_0": "22.",
  "anya-e0603n0:109_0": "Dvārāsayāvivariṃsu,",
  "anya-e0603n0:110_0": "Devatādvāramaṇḍalaṃ;",
  "anya-e0603n0:111_0": "Ratanukkāsahassāni,",
  "anya-e0603n0:112_0": "Jālayiṃsumarūtahiṃ.",
  "anya-e0603n0:113_0": "23.",
  "anya-e0603n0:114_0": "Mānikkhamassusiddhattha,",
  "anya-e0603n0:115_0": "Itotvaṃsattamedine;",
  "anya-e0603n0:116_0": "Bhavissasecakkavattī,",
  "anya-e0603n0:117_0": "Itinīvāripāpimā.",
  "anya-e0603n0:118_0": "24.",
  "anya-e0603n0:119_0": "Tagghajānāmitaṃmāra,",
  "anya-e0603n0:120_0": "Idhamātiṭṭhapakkama;",
  "anya-e0603n0:121_0": "Itimāraṃ palāpetvā,",
  "anya-e0603n0:122_0": "Vissaṭṭhosoabhikkami.",
  "anya-e0603n0:123_0": "25.",
  "anya-e0603n0:124_0": "Patvānomanadītīraṃ,",
  "anya-e0603n0:125_0": "Punāgamesituragaṃ;",
  "anya-e0603n0:126_0": "Channañcanagaraṃ bimbā;",
  "anya-e0603n0:127_0": "Devilāpaṭivedituṃ.",
  "anya-e0603n0:128_0": "26.",
  "anya-e0603n0:129_0": "Bandhitaṃyasodharāya,",
  "anya-e0603n0:130_0": "Moḷiṃsugandhavāsitaṃ;",
  "anya-e0603n0:131_0": "Chetvākhaggenatiṇhena,",
  "anya-e0603n0:132_0": "Ukkhipitthatamambare.",
  "anya-e0603n0:133_0": "27.",
  "anya-e0603n0:134_0": "Aṭṭhaseṭṭhaparikkhāre,",
  "anya-e0603n0:135_0": "Dhāretvābrahmunābhate;",
  "anya-e0603n0:136_0": "Hitvārājārahaṃdussaṃ,",
  "anya-e0603n0:137_0": "Samaṇatthamupāgami.",
  "anya-e0603n0:138_0": "28.",
  "anya-e0603n0:139_0": "Karontodukkaraṃkammaṃ,",
  "anya-e0603n0:140_0": "Chabbassāniparakkamo;",
  "anya-e0603n0:141_0": "Oḷārikannapānehi;",
  "anya-e0603n0:142_0": "Dehaṃyāpesiṭhītiyā.",
  "anya-e0603n0:143_0": "29.",
  "anya-e0603n0:144_0": "Visākhapuṇṇamehema,",
  "anya-e0603n0:145_0": "Pātiṃpāyasapūritaṃ;",
  "anya-e0603n0:146_0": "Sujātāyābhataṃgayha,",
  "anya-e0603n0:147_0": "Nerañcaramupāgami.",
  "anya-e0603n0:148_0": "30.",
  "anya-e0603n0:149_0": "Nerañcarāyatīramhi,",
  "anya-e0603n0:150_0": "Sutvāpāyāsamuttamaṃ;",
  "anya-e0603n0:151_0": "Paṭiyatthavaramaggena,",
  "anya-e0603n0:152_0": "Bodhimaṇḍamupāgami.",
  "anya-e0603n0:153_0": "31.",
  "anya-e0603n0:154_0": "Jayāsanepallaṅkamhi,",
  "anya-e0603n0:155_0": "Ṭhitodhiṭṭhesidhītimā;",
  "anya-e0603n0:156_0": "Navīriyaṃjahissāmi,",
  "anya-e0603n0:157_0": "Apattoitibuddhataṃ.",
  "anya-e0603n0:158_0": "32.",
  "anya-e0603n0:159_0": "Devindodhamayaṃsaṅkhaṃ,",
  "anya-e0603n0:160_0": "Setacchattaṃpitāmaho;",
  "anya-e0603n0:161_0": "Dhārayaṃvādentovīṇaṃ,",
  "anya-e0603n0:162_0": "Pañcasīkhoṭhitotahiṃ.",
  "anya-e0603n0:163_0": "33.",
  "anya-e0603n0:164_0": "Thutigītānigāyantī,",
  "anya-e0603n0:165_0": "Aṭṭhāsuṃdevasundarī;",
  "anya-e0603n0:166_0": "Gahetvāhemamañjūsā,",
  "anya-e0603n0:167_0": "Surapupphehipūritā.",
  "anya-e0603n0:168_0": "34.",
  "anya-e0603n0:169_0": "Anatthamentesūriye,",
  "anya-e0603n0:170_0": "Mārasenaṃpalāpayī;",
  "anya-e0603n0:171_0": "Aruṇuggamanekāle,",
  "anya-e0603n0:172_0": "Pattosibuddhataṃvaraṃ.",
  "anya-e0603n0:173_0": "35.",
  "anya-e0603n0:174_0": "Paṭṭhānaṃsammasantassa,",
  "anya-e0603n0:175_0": "Nāthassadehatosubhā;",
  "anya-e0603n0:176_0": "Chabbaṇṇabhānicchadhanti,",
  "anya-e0603n0:177_0": "Vidhāvanti tahiṃ tahiṃ.",
  "anya-e0603n0:178_0": "36.",
  "anya-e0603n0:179_0": "Nīlānīlāpitāpītā,",
  "anya-e0603n0:180_0": "Rattārattācanicchare;",
  "anya-e0603n0:181_0": "Setāsetācamañciṭṭhā,",
  "anya-e0603n0:182_0": "Tamhātamhāpabhassarā.",
  "anya-e0603n0:183_0": "37.",
  "anya-e0603n0:184_0": "Buddhapattopibodheyya,",
  "anya-e0603n0:185_0": "Dhammagambhīrataṃjino;",
  "anya-e0603n0:186_0": "Nisāmetvānadesetuṃ,",
  "anya-e0603n0:187_0": "Nirussāhamupāgato.",
  "anya-e0603n0:188_0": "38.",
  "anya-e0603n0:189_0": "Sahampatitadābrahmā,",
  "anya-e0603n0:190_0": "Desanāyābhiyācati;",
  "anya-e0603n0:191_0": "Tenasopathamaṃseṭṭhaṃ,",
  "anya-e0603n0:192_0": "Dhammacakkamadesayi.",
  "anya-e0603n0:193_0": "39.",
  "anya-e0603n0:194_0": "Ekavassovasambuddho,",
  "anya-e0603n0:195_0": "Varephagguṇapuṇṇame;",
  "anya-e0603n0:196_0": "Sakyānaṃnagaraṃgañchī,",
  "anya-e0603n0:197_0": "Sāvakehipurakkhato.",
  "anya-e0603n0:198_0": "40.",
  "anya-e0603n0:199_0": "Samāgamamhiñātīnaṃ,",
  "anya-e0603n0:200_0": "Vassāpesisusītalaṃ;",
  "anya-e0603n0:201_0": "Mahāmeghañcadesesi,",
  "anya-e0603n0:202_0": "Vessantarassajātakaṃ.",
  "anya-e0603n0:203_0": "41.",
  "anya-e0603n0:204_0": "Gantvāsāvakayugena,",
  "anya-e0603n0:205_0": "Yasodharāyamandiraṃ;",
  "anya-e0603n0:206_0": "Taṃtosetvānadesesi,",
  "anya-e0603n0:207_0": "Candakinnarajātakaṃ.",
  "anya-e0603n0:208_0": "42.",
  "anya-e0603n0:209_0": "Vasitvāsattamevasse,",
  "anya-e0603n0:210_0": "Tāvatiṃse silāsane;",
  "anya-e0603n0:211_0": "Abhidhammena tosesi,",
  "anya-e0603n0:212_0": "Devabrahmagaṇaṃbahuṃ.",
  "anya-e0603n0:213_0": "43.",
  "anya-e0603n0:214_0": "Pattharaṃñāṇajālena,",
  "anya-e0603n0:215_0": "Lokaṃlokesisabbadā;",
  "anya-e0603n0:216_0": "Bodhetuṃbodhiyābhāya;",
  "anya-e0603n0:217_0": "Bodheyyajanapaṅkajaṃ.",
  "anya-e0603n0:218_0": "44.",
  "anya-e0603n0:219_0": "Bodhaneyyaṃjanaṃdisvā,",
  "anya-e0603n0:220_0": "Cakkavāḷaparaṃparā;",
  "anya-e0603n0:221_0": "Gantvāpivibodhesi,",
  "anya-e0603n0:222_0": "Buddhaseṭṭhosubodhimā.",
  "anya-e0603n0:223_0": "45.",
  "anya-e0603n0:224_0": "Bakādikecabrahmāno,",
  "anya-e0603n0:225_0": "Yakkhecāḷavakādike;",
  "anya-e0603n0:226_0": "Purindadādikedeve,",
  "anya-e0603n0:227_0": "Sūcilomādirakkhase.",
  "anya-e0603n0:228_0": "46.",
  "anya-e0603n0:229_0": "Jāṇusoṇādikeneke,",
  "anya-e0603n0:230_0": "Brāhmaṇecāvikakkhaḷe;",
  "anya-e0603n0:231_0": "Cūḷodarādikenāge,",
  "anya-e0603n0:232_0": "Dhanapālādikegaje.",
  "anya-e0603n0:233_0": "47.",
  "anya-e0603n0:234_0": "Buddhāmayaṃtimaññante,",
  "anya-e0603n0:235_0": "Titthiyenekaladdhike;",
  "anya-e0603n0:236_0": "Damesidamataṃseṭṭho,",
  "anya-e0603n0:237_0": "Paravādappamaddano.",
  "anya-e0603n0:238_0": "48.",
  "anya-e0603n0:239_0": "Anubyañjanasampanno,",
  "anya-e0603n0:240_0": "Esabāttiṃsalakkhaṇo;",
  "anya-e0603n0:241_0": "Byāmappabhāparikkhitto,",
  "anya-e0603n0:242_0": "Ketumālāsilaṅkhato.",
  "anya-e0603n0:243_0": "49.",
  "anya-e0603n0:244_0": "Caturāsītisahassa,",
  "anya-e0603n0:245_0": "Dhammakkhandhesadesayi;",
  "anya-e0603n0:246_0": "Pañcatāthīsavassāni,",
  "anya-e0603n0:247_0": "Tilokassahitāyave.",
  "anya-e0603n0:248_0": "50.",
  "anya-e0603n0:249_0": "Buddhakiccaṃkarontoso,",
  "anya-e0603n0:250_0": "Vahaṃlokahitaṃvaraṃ;",
  "anya-e0603n0:251_0": "Mocayaṃbandhanāsatte,",
  "anya-e0603n0:252_0": "Kālaṃkhepetisabbadā.",
  "anya-e0603n0:253_0": "51.",
  "anya-e0603n0:254_0": "Yahiṃ āsītikonātho,",
  "anya-e0603n0:255_0": "Nibbutovaramañjake;",
  "anya-e0603n0:256_0": "Mahāsālavaneramme,",
  "anya-e0603n0:257_0": "Mallānaṃnigametahiṃ.",
  "anya-e0603n0:258_0": "52.",
  "anya-e0603n0:259_0": "Aggikkhandhovalokasmiṃ,",
  "anya-e0603n0:260_0": "Jāletvānasasāvako;",
  "anya-e0603n0:261_0": "Nibbāyitthasaṅkhatānaṃ,",
  "anya-e0603n0:262_0": "Pakāsentoaniccataṃti.",
  "anya-e0603n0:263_0": "“Namotebhagavānātha,",
  "anya-e0603n0:264_0": "Namotejayataṃjaya;",
  "anya-e0603n0:265_0": "Namotesirisampanna,",
  "anya-e0603n0:266_0": "Namotelokanandana.”",
  "anya-e0603n0:267_0": "Buddhavandanā",
  "anya-e0603n0:268_0": "1.",
  "anya-e0603n0:269_0": "Devalokā cavitvāna,",
  "anya-e0603n0:270_0": "Mahāmāyāya kucchiyaṃ;",
  "anya-e0603n0:271_0": "Uppajji guruvāramhi,",
  "anya-e0603n0:272_0": "Vandetaṃ sakyapuṅgavaṃ.",
  "anya-e0603n0:273_0": "2.",
  "anya-e0603n0:274_0": "Dasamā saccaye neso,",
  "anya-e0603n0:275_0": "Vijāyi mātukucchito;",
  "anya-e0603n0:276_0": "Sukkavāre lumbiniyaṃ,",
  "anya-e0603n0:277_0": "Vande taṃ lokapūjitaṃ.",
  "anya-e0603n0:278_0": "3.",
  "anya-e0603n0:279_0": "Cakkavattisiriṃ hitvā,",
  "anya-e0603n0:280_0": "Mahāsinikkhamaṃ sudhī;",
  "anya-e0603n0:281_0": "Nikkhamī candavāramhi,",
  "anya-e0603n0:282_0": "Name taṃ munikuñjaraṃ.",
  "anya-e0603n0:283_0": "4.",
  "anya-e0603n0:284_0": "Assatthabodhimūlamhi,",
  "anya-e0603n0:285_0": "Pallaṅke aparājite;",
  "anya-e0603n0:286_0": "Patto sabbaññutaṃ nātho,",
  "anya-e0603n0:287_0": "Name taṃ budhavāsare.",
  "anya-e0603n0:288_0": "5.",
  "anya-e0603n0:289_0": "Pañcavaggiyamukhānaṃ,",
  "anya-e0603n0:290_0": "Devānaṃ migadāvane;",
  "anya-e0603n0:291_0": "Sorivāre dhammacakkaṃ,",
  "anya-e0603n0:292_0": "Vattesi taṃ jinaṃ name.",
  "anya-e0603n0:293_0": "6.",
  "anya-e0603n0:294_0": "Sabbasaṅkhatadhammānaṃ,",
  "anya-e0603n0:295_0": "Pakāsiyaaniccataṃ;",
  "anya-e0603n0:296_0": "Nibbutaṃaṅgavāramhi,",
  "anya-e0603n0:297_0": "Namādhiamatandadaṃ.",
  "anya-e0603n0:298_0": "7.",
  "anya-e0603n0:299_0": "Sūriyavāre soṇṇāya,",
  "anya-e0603n0:300_0": "Doṇiyā jinaviggaho;",
  "anya-e0603n0:301_0": "Agginujjalito tassa,",
  "anya-e0603n0:302_0": "Name dhātusarīrakaṃ.",
  "anya-e0603n0:303_0": "Uṇṇālomikanātha vandanā",
  "anya-e0603n0:304_0": "Uṇṇālodhikanāthassa,",
  "anya-e0603n0:305_0": "Uṇṇāya bhamukantare;",
  "anya-e0603n0:306_0": "Vajirāviya sobhanti,",
  "anya-e0603n0:307_0": "Nikkhanti yo supaṇḍarā.",
  "anya-e0603n0:308_0": "Vajiragghanakāyassa,",
  "anya-e0603n0:309_0": "Nāthassa dehato subhā;",
  "anya-e0603n0:310_0": "Vajireyyā niccharanti;",
  "anya-e0603n0:311_0": "Vijjūva gagaṇantare.",
  "anya-e0603n0:312_0": "Rinindamandahāsena,",
  "anya-e0603n0:313_0": "Dāṭhāya niccharā pabhā;",
  "anya-e0603n0:314_0": "Vipphūrisā disāsabbā,",
  "anya-e0603n0:315_0": "Vajirāyanti paṇḍarā."
}