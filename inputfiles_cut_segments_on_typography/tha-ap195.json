{
  "tha-ap195:0.1_0": "Therāpadāna",
  "tha-ap195:0.2_0": "Tamālapupphiyavagga",
  "tha-ap195:0.3_0": "3. Khaṇḍaphulliyattheraapadāna",
  "tha-ap195:1.1_0": "“Phussassa kho bhagavato,",
  "tha-ap195:1.2_0": "thūpo āsi mahāvane;",
  "tha-ap195:1.3_0": "Kuñjarehi tadā bhinno,",
  "tha-ap195:1.4_0": "parūḷho pādapo tahiṁ.",
  "tha-ap195:2.1_0": "Visamañca samaṁ katvā,",
  "tha-ap195:2.2_0": "sudhāpiṇḍaṁ adāsahaṁ;",
  "tha-ap195:2.3_0": "Tilokagaruno tassa,",
  "tha-ap195:2.4_0": "guṇehi paritosito.",
  "tha-ap195:3.1_0": "Dvenavute ito kappe,",
  "tha-ap195:3.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap195:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap195:3.4_0": "sudhāpiṇḍassidaṁ phalaṁ.",
  "tha-ap195:4.1_0": "Sattasattatikappamhi,",
  "tha-ap195:4.2_0": "jitasenāsuṁ soḷasa;",
  "tha-ap195:4.3_0": "Sattaratanasampannā,",
  "tha-ap195:4.4_0": "cakkavattī mahabbalā.",
  "tha-ap195:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap195:5.2_0": "…pe…",
  "tha-ap195:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap195:6.1_0": "Itthaṁ sudaṁ āyasmā khaṇḍaphulliyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap195:7.1_0": "Khaṇḍaphulliyattherassāpadānaṁ tatiyaṁ."
}