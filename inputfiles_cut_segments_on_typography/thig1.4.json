{
  "thig1.4:0.1_0": "Therīgāthā 1.4",
  "thig1.4:0.2_0": "Ekakanipāta",
  "thig1.4:0.3_0": "Tissātherīgāthā",
  "thig1.4:1.1_0": "“Tisse sikkhassu sikkhāya,",
  "thig1.4:1.2_0": "mā taṁ yogā upaccaguṁ;",
  "thig1.4:1.3_0": "Sabbayogavisaṁyuttā,",
  "thig1.4:1.4_0": "cara loke anāsavā”ti.",
  "thig1.4:2.1_0": "… Tissā therī …."
}