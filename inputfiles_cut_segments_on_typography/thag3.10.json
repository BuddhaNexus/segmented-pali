{
  "thag3.10:0.1_0": "Theragāthā 3.10",
  "thag3.10:0.2_0": "Tikanipāta",
  "thag3.10:0.3_0": "Paṭhamavagga",
  "thag3.10:0.4_0": "Sāṭimattiyattheragāthā",
  "thag3.10:1.1_0": "“Ahu tuyhaṁ pure saddhā,",
  "thag3.10:1.2_0": "sā te ajja na vijjati;",
  "thag3.10:1.3_0": "Yaṁ tuyhaṁ tuyhamevetaṁ,",
  "thag3.10:1.4_0": "natthi duccaritaṁ mama.",
  "thag3.10:2.1_0": "Aniccā hi calā saddhā,",
  "thag3.10:2.2_0": "evaṁ diṭṭhā hi sā mayā;",
  "thag3.10:2.3_0": "Rajjantipi virajjanti,",
  "thag3.10:2.4_0": "tattha kiṁ jiyyate muni.",
  "thag3.10:3.1_0": "Paccati munino bhattaṁ,",
  "thag3.10:3.2_0": "Thokaṁ thokaṁ kule kule;",
  "thag3.10:3.3_0": "Piṇḍikāya carissāmi,",
  "thag3.10:3.4_0": "Atthi jaṅghabalaṁ mamā”ti.",
  "thag3.10:3.5_0": "… Sāṭimattiyo thero …."
}