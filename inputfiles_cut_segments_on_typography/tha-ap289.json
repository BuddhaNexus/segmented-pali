{
  "tha-ap289:0.1_0": "Therāpadāna",
  "tha-ap289:0.2_0": "Paṇṇadāyakavagga",
  "tha-ap289:0.3_0": "7. Apadāniyattheraapadāna",
  "tha-ap289:1.1_0": "“Apadānaṁ sugatānaṁ,",
  "tha-ap289:1.2_0": "kittayiṁhaṁ mahesinaṁ;",
  "tha-ap289:1.3_0": "Pāde ca sirasā vandiṁ,",
  "tha-ap289:1.4_0": "pasanno sehi pāṇibhi.",
  "tha-ap289:2.1_0": "Dvenavute ito kappe,",
  "tha-ap289:2.2_0": "apadānaṁ pakittayiṁ;",
  "tha-ap289:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap289:2.4_0": "kittanāya idaṁ phalaṁ.",
  "tha-ap289:3.1_0": "Paṭisambhidā catasso,",
  "tha-ap289:3.2_0": "…pe…",
  "tha-ap289:3.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap289:4.1_0": "Itthaṁ sudaṁ āyasmā apadāniyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap289:5.1_0": "Apadāniyattherassāpadānaṁ sattamaṁ."
}