{
  "ja51:0.1_0": "Jātaka",
  "ja51:0.2_0": "Ekakanipāta",
  "ja51:0.3_0": "Āsīsavagga",
  "ja51:0.4_0": "1. Mahāsīlavajātaka",
  "ja51:1.1_0": "“Āsīsetheva puriso,",
  "ja51:1.2_0": "na nibbindeyya paṇḍito;",
  "ja51:1.3_0": "Passāmi vohaṁ attānaṁ,",
  "ja51:1.4_0": "yathā icchiṁ tathā ahū”ti.",
  "ja51:2.1_0": "Mahāsīlavajātakaṁ paṭhamaṁ."
}