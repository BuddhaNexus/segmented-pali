{
  "tha-ap111:0.1_0": "Therāpadāna",
  "tha-ap111:0.2_0": "Bhikkhadāyivagga",
  "tha-ap111:0.3_0": "9. Biḷālidāyakattheraapadāna",
  "tha-ap111:1.1_0": "“Himavantassāvidūre,",
  "tha-ap111:1.2_0": "vasāmi paṇṇasanthare;",
  "tha-ap111:1.3_0": "Ghāsesu gedhamāpanno,",
  "tha-ap111:1.4_0": "seyyasīlo cahaṁ tadā.",
  "tha-ap111:2.1_0": "Khaṇantālu kalambāni,",
  "tha-ap111:2.2_0": "biḷālitakkalāni ca;",
  "tha-ap111:2.3_0": "Kolaṁ bhallātakaṁ billaṁ,",
  "tha-ap111:2.4_0": "āhatvā paṭiyāditaṁ.",
  "tha-ap111:3.1_0": "Padumuttaro lokavidū,",
  "tha-ap111:3.2_0": "āhutīnaṁ paṭiggaho;",
  "tha-ap111:3.3_0": "Mama saṅkappamaññāya,",
  "tha-ap111:3.4_0": "āgacchi mama santikaṁ.",
  "tha-ap111:4.1_0": "Upāgataṁ mahānāgaṁ,",
  "tha-ap111:4.2_0": "devadevaṁ narāsabhaṁ;",
  "tha-ap111:4.3_0": "Biḷāliṁ paggahetvāna,",
  "tha-ap111:4.4_0": "pattamhi okiriṁ ahaṁ.",
  "tha-ap111:5.1_0": "Paribhuñji mahāvīro,",
  "tha-ap111:5.2_0": "tosayanto mamaṁ tadā;",
  "tha-ap111:5.3_0": "Paribhuñjitvāna sabbaññū,",
  "tha-ap111:5.4_0": "imaṁ gāthaṁ abhāsatha.",
  "tha-ap111:6.1_0": "‘Sakaṁ cittaṁ pasādetvā,",
  "tha-ap111:6.2_0": "biḷāliṁ me adā tuvaṁ;",
  "tha-ap111:6.3_0": "Kappānaṁ satasahassaṁ,",
  "tha-ap111:6.4_0": "duggatiṁ nupapajjasi’.",
  "tha-ap111:7.1_0": "Carimaṁ vattate mayhaṁ,",
  "tha-ap111:7.2_0": "bhavā sabbe samūhatā;",
  "tha-ap111:7.3_0": "Dhāremi antimaṁ dehaṁ,",
  "tha-ap111:7.4_0": "sammāsambuddhasāsane.",
  "tha-ap111:8.1_0": "Catupaññāsito kappe,",
  "tha-ap111:8.2_0": "sumekhaliyasavhayo;",
  "tha-ap111:8.3_0": "Sattaratanasampanno,",
  "tha-ap111:8.4_0": "cakkavattī mahabbalo.",
  "tha-ap111:9.1_0": "Paṭisambhidā catasso,",
  "tha-ap111:9.2_0": "…pe…",
  "tha-ap111:9.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap111:10.1_0": "Itthaṁ sudaṁ āyasmā biḷālidāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap111:11.1_0": "Biḷālidāyakattherassāpadānaṁ navamaṁ."
}