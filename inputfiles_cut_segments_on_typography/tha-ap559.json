{
  "tha-ap559:0.1_0": "Therāpadāna",
  "tha-ap559:0.2_0": "Yasavagga",
  "tha-ap559:0.3_0": "7. Aparauttarattheraapadāna",
  "tha-ap559:1.1_0": "“Nibbute lokanāthamhi,",
  "tha-ap559:1.2_0": "siddhatthe lokanāyake;",
  "tha-ap559:1.3_0": "Mama ñātī samānetvā,",
  "tha-ap559:1.4_0": "dhātupūjaṁ akāsahaṁ.",
  "tha-ap559:2.1_0": "Catunnavutito kappe,",
  "tha-ap559:2.2_0": "yaṁ dhātumabhipūjayiṁ;",
  "tha-ap559:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap559:2.4_0": "dhātupūjāyidaṁ phalaṁ.",
  "tha-ap559:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap559:3.2_0": "…pe…",
  "tha-ap559:3.3_0": "viharāmi anāsavo.",
  "tha-ap559:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap559:4.2_0": "…pe…",
  "tha-ap559:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap559:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap559:5.2_0": "…pe…",
  "tha-ap559:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap559:6.1_0": "Itthaṁ sudaṁ āyasmā aparauttaratthero imā gāthāyo abhāsitthāti.",
  "tha-ap559:7.1_0": "Aparassa uttarattherassāpadānaṁ sattamaṁ."
}