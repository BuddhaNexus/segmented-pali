{
  "ja527:0.1_0": "Jātaka",
  "ja527:0.2_0": "Paṇṇāsanipāta",
  "ja527:0.3_0": "Niḷinikāvagga",
  "ja527:0.4_0": "2 Ummādantījātaka",
  "ja527:1.1_0": "“Nivesanaṁ kassa nudaṁ sunanda,",
  "ja527:1.2_0": "Pākārena paṇḍumayena guttaṁ;",
  "ja527:1.3_0": "Kā dissati aggisikhāva dūre,",
  "ja527:1.4_0": "Vehāyasaṁ pabbataggeva acci.",
  "ja527:2.1_0": "Dhītā nvayaṁ kassa sunanda hoti,",
  "ja527:2.2_0": "Suṇisā nvayaṁ kassa athopi bhariyā;",
  "ja527:2.3_0": "Akkhāhi me khippamidheva puṭṭho,",
  "ja527:2.4_0": "Avāvaṭā yadi vā atthi bhattā”.",
  "ja527:3.1_0": "“Ahañhi jānāmi janinda etaṁ,",
  "ja527:3.2_0": "Matyā ca petyā ca athopi assā;",
  "ja527:3.3_0": "Taveva so puriso bhūmipāla,",
  "ja527:3.4_0": "Rattindivaṁ appamatto tavatthe.",
  "ja527:4.1_0": "Iddho ca phīto ca suvaḍḍhito ca,",
  "ja527:4.2_0": "Amacco ca te aññataro janinda;",
  "ja527:4.3_0": "Tassesā bhariyābhipārakassa,",
  "ja527:4.4_0": "Ummādantī nāmadheyyena rāja”.",
  "ja527:5.1_0": "“Ambho ambho nāmamidaṁ imissā,",
  "ja527:5.2_0": "Matyā ca petyā ca kataṁ susādhu;",
  "ja527:5.3_0": "Tadā hi mayhaṁ avalokayantī,",
  "ja527:5.4_0": "Ummattakaṁ ummadantī akāsi.",
  "ja527:6.1_0": "Yā puṇṇamāse migamandalocanā,",
  "ja527:6.2_0": "Upāvisi puṇḍarīkattacaṅgī;",
  "ja527:6.3_0": "Dve puṇṇamāyo tadahū amaññahaṁ,",
  "ja527:6.4_0": "Disvāna pārāvatarattavāsiniṁ.",
  "ja527:7.1_0": "Aḷārapamhehi subhehi vaggubhi,",
  "ja527:7.2_0": "Palobhayantī maṁ yadā udikkhati;",
  "ja527:7.3_0": "Vijambhamānā harateva me mano,",
  "ja527:7.4_0": "Jātā vane kimpurisīva pabbate.",
  "ja527:8.1_0": "Tadā hi brahatī sāmā,",
  "ja527:8.2_0": "āmuttamaṇikuṇḍalā;",
  "ja527:8.3_0": "Ekaccavasanā nārī,",
  "ja527:8.4_0": "migī bhantāvudikkhati.",
  "ja527:9.1_0": "Kadāssu maṁ tambanakhā sulomā,",
  "ja527:9.2_0": "Bāhāmudū candanasāralittā;",
  "ja527:9.3_0": "Vaṭṭaṅgulī sannatadhīrakuttiyā,",
  "ja527:9.4_0": "Nārī upaññissati sīsato subhā.",
  "ja527:10.1_0": "Kadāssu maṁ kañcanajāluracchadā,",
  "ja527:10.2_0": "Dhītā tirīṭissa vilaggamajjhā;",
  "ja527:10.3_0": "Mudūhi bāhāhi palissajissati,",
  "ja527:10.4_0": "Brahāvane jātadumaṁva māluvā.",
  "ja527:11.1_0": "Kadāssu lākhārasarattasucchavī,",
  "ja527:11.2_0": "Bindutthanī puṇḍarīkattacaṅgī;",
  "ja527:11.3_0": "Mukhaṁ mukhena upanāmayissati,",
  "ja527:11.4_0": "Soṇḍova soṇḍassa surāya thālaṁ.",
  "ja527:12.1_0": "Yadāddasaṁ taṁ tiṭṭhantiṁ,",
  "ja527:12.2_0": "Sabbabhaddaṁ manoramaṁ;",
  "ja527:12.3_0": "Tato sakassa cittassa,",
  "ja527:12.4_0": "Nāvabodhāmi kañcinaṁ.",
  "ja527:13.1_0": "Ummādantimahaṁ daṭṭhā,",
  "ja527:13.2_0": "āmuttamaṇikuṇḍalaṁ;",
  "ja527:13.3_0": "Na supāmi divārattiṁ,",
  "ja527:13.4_0": "sahassaṁva parājito.",
  "ja527:14.1_0": "Sakko ce me varaṁ dajjā,",
  "ja527:14.2_0": "so ca labbhetha me varo;",
  "ja527:14.3_0": "Ekarattaṁ dvirattaṁ vā,",
  "ja527:14.4_0": "bhaveyyaṁ abhipārako;",
  "ja527:14.5_0": "Ummādantyā ramitvāna,",
  "ja527:14.6_0": "sivirājā tato siyaṁ”.",
  "ja527:15.1_0": "“Bhūtāni me bhūtapatī namassato,",
  "ja527:15.2_0": "Āgamma yakkho idametadabravi;",
  "ja527:15.3_0": "Rañño mano ummadantyā niviṭṭho,",
  "ja527:15.4_0": "Dadāmi te taṁ paricārayassu”.",
  "ja527:16.1_0": "“Puññā ca dhaṁse amaro na camhi,",
  "ja527:16.2_0": "Jano ca me pāpamidañca jaññā;",
  "ja527:16.3_0": "Bhuso ca tyassa manaso vighāto,",
  "ja527:16.4_0": "Datvā piyaṁ ummadantiṁ adaṭṭhā”.",
  "ja527:17.1_0": "“Janinda nāññatra tayā mayā vā,",
  "ja527:17.2_0": "Sabbāpi kammassa katassa jaññā;",
  "ja527:17.3_0": "Yaṁ te mayā ummadantī padinnā,",
  "ja527:17.4_0": "Bhusehi rājā vanathaṁ sajāhi”.",
  "ja527:18.1_0": "“Yo pāpakaṁ kamma karaṁ manusso,",
  "ja527:18.2_0": "So maññati māyida maññiṁsu aññe;",
  "ja527:18.3_0": "Passanti bhūtāni karontametaṁ,",
  "ja527:18.4_0": "Yuttā ca ye honti narā pathabyā.",
  "ja527:19.1_0": "Añño nu te koci naro pathabyā,",
  "ja527:19.2_0": "Saddheyya lokasmi na me piyāti;",
  "ja527:19.3_0": "Bhuso ca tyassa manaso vighāto,",
  "ja527:19.4_0": "Datvā piyaṁ ummadantiṁ adaṭṭhā”.",
  "ja527:20.1_0": "“Addhā piyā mayha janinda esā,",
  "ja527:20.2_0": "Na sā mamaṁ appiyā bhūmipāla;",
  "ja527:20.3_0": "Gaccheva tvaṁ ummadantiṁ bhadante,",
  "ja527:20.4_0": "Sīhova selassa guhaṁ upeti”.",
  "ja527:21.1_0": "“Na pīḷitā attadukhena dhīrā,",
  "ja527:21.2_0": "Sukhapphalaṁ kamma pariccajanti;",
  "ja527:21.3_0": "Sammohitā vāpi sukhena mattā,",
  "ja527:21.4_0": "Na pāpakammañca samācaranti”.",
  "ja527:22.1_0": "“Tuvañhi mātā ca pitā ca mayhaṁ,",
  "ja527:22.2_0": "Bhattā patī posako devatā ca;",
  "ja527:22.3_0": "Dāso ahaṁ tuyha saputtadāro,",
  "ja527:22.4_0": "Yathāsukhaṁ sāmi karohi kāmaṁ”.",
  "ja527:23.1_0": "“Yo issaromhīti karoti pāpaṁ,",
  "ja527:23.2_0": "Katvā ca so nuttasate paresaṁ;",
  "ja527:23.3_0": "Na tena so jīvati dīghamāyu,",
  "ja527:23.4_0": "Devāpi pāpena samekkhare naṁ.",
  "ja527:24.1_0": "Aññātakaṁ sāmikehī padinnaṁ,",
  "ja527:24.2_0": "Dhamme ṭhitā ye paṭicchanti dānaṁ;",
  "ja527:24.3_0": "Paṭicchakā dāyakā cāpi tattha,",
  "ja527:24.4_0": "Sukhapphalaññeva karonti kammaṁ.",
  "ja527:25.1_0": "Añño nu te koci naro pathabyā,",
  "ja527:25.2_0": "Saddheyya lokasmi na me piyāti;",
  "ja527:25.3_0": "Bhuso ca tyassa manaso vighāto,",
  "ja527:25.4_0": "Datvā piyaṁ ummadantiṁ adaṭṭhā”.",
  "ja527:26.1_0": "“Addhā piyā mayha janinda esā,",
  "ja527:26.2_0": "Na sā mamaṁ appiyā bhūmipāla;",
  "ja527:26.3_0": "Yaṁ te mayā ummadantī padinnā,",
  "ja527:26.4_0": "Bhusehi rājā vanathaṁ sajāhi”.",
  "ja527:27.1_0": "“Yo attadukkhena parassa dukkhaṁ,",
  "ja527:27.2_0": "Sukhena vā attasukhaṁ dahāti;",
  "ja527:27.3_0": "Yathevidaṁ mayha tathā paresaṁ,",
  "ja527:27.4_0": "Yo evaṁ jānāti sa vedi dhammaṁ.",
  "ja527:28.1_0": "Añño nu te koci naro pathabyā,",
  "ja527:28.2_0": "Saddheyya lokasmi na me piyāti;",
  "ja527:28.3_0": "Bhuso ca tyassa manaso vighāto,",
  "ja527:28.4_0": "Datvā piyaṁ ummadantiṁ adaṭṭhā”.",
  "ja527:29.1_0": "“Janinda jānāsi piyā mamesā,",
  "ja527:29.2_0": "Na sā mamaṁ appiyā bhūmipāla;",
  "ja527:29.3_0": "Piyena te dammi piyaṁ janinda,",
  "ja527:29.4_0": "Piyadāyino deva piyaṁ labhanti”.",
  "ja527:30.1_0": "“So nūnāhaṁ vadhissāmi,",
  "ja527:30.2_0": "attānaṁ kāmahetukaṁ;",
  "ja527:30.3_0": "Na hi dhammaṁ adhammena,",
  "ja527:30.4_0": "ahaṁ vadhitumussahe”.",
  "ja527:31.1_0": "“Sace tuvaṁ mayha satiṁ janinda,",
  "ja527:31.2_0": "Na kāmayāsi naravīra seṭṭha;",
  "ja527:31.3_0": "Cajāmi naṁ sabbajanassa sibyā,",
  "ja527:31.4_0": "Mayā pamuttaṁ tato avhayesi naṁ”.",
  "ja527:32.1_0": "“Adūsiyañce abhipāraka tvaṁ,",
  "ja527:32.2_0": "Cajāsi katte ahitāya tyassa;",
  "ja527:32.3_0": "Mahā ca te upavādopi assa,",
  "ja527:32.4_0": "Na cāpi tyassa nagaramhi pakkho”.",
  "ja527:33.1_0": "“Ahaṁ sahissaṁ upavādametaṁ,",
  "ja527:33.2_0": "Nindaṁ pasaṁsaṁ garahañca sabbaṁ;",
  "ja527:33.3_0": "Mametamāgacchatu bhūmipāla,",
  "ja527:33.4_0": "Yathāsukhaṁ sivi karohi kāmaṁ”.",
  "ja527:34.1_0": "“Yo neva nindaṁ na panappasaṁsaṁ,",
  "ja527:34.2_0": "Ādiyati garahaṁ nopi pūjaṁ;",
  "ja527:34.3_0": "Sirī ca lakkhī ca apeti tamhā,",
  "ja527:34.4_0": "Āpo suvuṭṭhīva yathā thalamhā”.",
  "ja527:35.1_0": "“Yaṁ kiñci dukkhañca sukhañca etto,",
  "ja527:35.2_0": "Dhammātisārañca manovighātaṁ;",
  "ja527:35.3_0": "Urasā ahaṁ paccuttarissāmi sabbaṁ,",
  "ja527:35.4_0": "Pathavī yathā thāvarānaṁ tasānaṁ”.",
  "ja527:36.1_0": "“Dhammātisārañca manovighātaṁ,",
  "ja527:36.2_0": "Dukkhañca nicchāmi ahaṁ paresaṁ;",
  "ja527:36.3_0": "Ekovimaṁ hārayissāmi bhāraṁ,",
  "ja527:36.4_0": "Dhamme ṭhito kiñci ahāpayanto”.",
  "ja527:37.1_0": "“Saggūpagaṁ puññakammaṁ janinda,",
  "ja527:37.2_0": "Mā me tuvaṁ antarāyaṁ akāsi;",
  "ja527:37.3_0": "Dadāmi te ummadantiṁ pasanno,",
  "ja527:37.4_0": "Rājāva yaññe dhanaṁ brāhmaṇānaṁ”.",
  "ja527:38.1_0": "“Addhā tuvaṁ katte hitesi mayhaṁ,",
  "ja527:38.2_0": "Sakhā mamaṁ ummadantī tuvañca;",
  "ja527:38.3_0": "Nindeyyu devā pitaro ca sabbe,",
  "ja527:38.4_0": "Pāpañca passaṁ abhisamparāyaṁ”.",
  "ja527:39.1_0": "“Na hetadhammaṁ sivirāja vajjuṁ,",
  "ja527:39.2_0": "Sanegamā jānapadā ca sabbe;",
  "ja527:39.3_0": "Yaṁ te mayā ummadantī padinnā,",
  "ja527:39.4_0": "Bhusehi rājā vanathaṁ sajāhi”.",
  "ja527:40.1_0": "“Addhā tuvaṁ katte hitesi mayhaṁ,",
  "ja527:40.2_0": "Sakhā mamaṁ ummadantī tuvañca;",
  "ja527:40.3_0": "Satañca dhammāni sukittitāni,",
  "ja527:40.4_0": "Samuddavelāva duraccayāni”.",
  "ja527:41.1_0": "“Āhuneyyo mesi hitānukampī,",
  "ja527:41.2_0": "Dhātā vidhātā casi kāmapālo;",
  "ja527:41.3_0": "Tayī hutā rāja mahapphalā hi,",
  "ja527:41.4_0": "Kāmena me ummadantiṁ paṭiccha”.",
  "ja527:42.1_0": "“Addhā hi sabbaṁ abhipāraka tvaṁ,",
  "ja527:42.2_0": "Dhammaṁ acārī mama kattuputta;",
  "ja527:42.3_0": "Añño nu te ko idha sotthikattā,",
  "ja527:42.4_0": "Dvipado naro aruṇe jīvaloke”.",
  "ja527:43.1_0": "“Tuvaṁ nu seṭṭho tvamanuttarosi,",
  "ja527:43.2_0": "Tvaṁ dhammagutto dhammavidū sumedho;",
  "ja527:43.3_0": "So dhammagutto cirameva jīva,",
  "ja527:43.4_0": "Dhammañca me desaya dhammapāla”.",
  "ja527:44.1_0": "“Tadiṅgha abhipāraka,",
  "ja527:44.2_0": "suṇohi vacanaṁ mama;",
  "ja527:44.3_0": "Dhammaṁ te desayissāmi,",
  "ja527:44.4_0": "sataṁ āsevitaṁ ahaṁ.",
  "ja527:45.1_0": "Sādhu dhammaruci rājā,",
  "ja527:45.2_0": "sādhu paññāṇavā naro;",
  "ja527:45.3_0": "Sādhu mittānamaddubbho,",
  "ja527:45.4_0": "pāpassākaraṇaṁ sukhaṁ.",
  "ja527:46.1_0": "Akkodhanassa vijite,",
  "ja527:46.2_0": "ṭhitadhammassa rājino;",
  "ja527:46.3_0": "Sukhaṁ manussā āsetha,",
  "ja527:46.4_0": "sītacchāyāya saṅghare.",
  "ja527:47.1_0": "Na cāhametaṁ abhirocayāmi,",
  "ja527:47.2_0": "Kammaṁ asamekkhakataṁ asādhu;",
  "ja527:47.3_0": "Ye vāpi ñatvāna sayaṁ karonti,",
  "ja527:47.4_0": "Upamā imā mayhaṁ tuvaṁ suṇohi.",
  "ja527:48.1_0": "Gavañce taramānānaṁ,",
  "ja527:48.2_0": "jimhaṁ gacchati puṅgavo;",
  "ja527:48.3_0": "Sabbā tā jimhaṁ gacchanti,",
  "ja527:48.4_0": "nette jimhaṁ gate sati.",
  "ja527:49.1_0": "Evameva manussesu,",
  "ja527:49.2_0": "yo hoti seṭṭhasammato;",
  "ja527:49.3_0": "So ce adhammaṁ carati,",
  "ja527:49.4_0": "pageva itarā pajā;",
  "ja527:49.5_0": "Sabbaṁ raṭṭhaṁ dukhaṁ seti,",
  "ja527:49.6_0": "rājā ce hoti adhammiko.",
  "ja527:50.1_0": "Gavañce taramānānaṁ,",
  "ja527:50.2_0": "ujuṁ gacchati puṅgavo;",
  "ja527:50.3_0": "Sabbā gāvī ujuṁ yanti,",
  "ja527:50.4_0": "nette ujuṁ gate sati.",
  "ja527:51.1_0": "Evameva manussesu,",
  "ja527:51.2_0": "yo hoti seṭṭhasammato;",
  "ja527:51.3_0": "So sace dhammaṁ carati,",
  "ja527:51.4_0": "pageva itarā pajā;",
  "ja527:51.5_0": "Sabbaṁ raṭṭhaṁ sukhaṁ seti,",
  "ja527:51.6_0": "rājā ce hoti dhammiko.",
  "ja527:52.1_0": "Na cāpāhaṁ adhammena,",
  "ja527:52.2_0": "amarattamabhipatthaye;",
  "ja527:52.3_0": "Imaṁ vā pathaviṁ sabbaṁ,",
  "ja527:52.4_0": "vijetuṁ abhipāraka.",
  "ja527:53.1_0": "Yañhi kiñci manussesu,",
  "ja527:53.2_0": "ratanaṁ idha vijjati;",
  "ja527:53.3_0": "Gāvo dāso hiraññañca,",
  "ja527:53.4_0": "vatthiyaṁ haricandanaṁ.",
  "ja527:54.1_0": "Assitthiyo ratanaṁ maṇikañca,",
  "ja527:54.2_0": "Yañcāpi me candasūriyā abhipālayanti;",
  "ja527:54.3_0": "Na tassa hetu visamaṁ careyyaṁ,",
  "ja527:54.4_0": "Majjhe sivīnaṁ usabhomhi jāto.",
  "ja527:55.1_0": "Netā hitā uggato raṭṭhapālo,",
  "ja527:55.2_0": "Dhammaṁ sivīnaṁ apacāyamāno;",
  "ja527:55.3_0": "So dhammamevānuvicintayanto,",
  "ja527:55.4_0": "Tasmā sake cittavase na vatto”.",
  "ja527:56.1_0": "“Addhā tuvaṁ mahārāja,",
  "ja527:56.2_0": "niccaṁ abyasanaṁ sivaṁ;",
  "ja527:56.3_0": "Karissasi ciraṁ rajjaṁ,",
  "ja527:56.4_0": "paññā hi tava tādisī.",
  "ja527:57.1_0": "Etaṁ te anumodāma,",
  "ja527:57.2_0": "yaṁ dhammaṁ nappamajjasi;",
  "ja527:57.3_0": "Dhammaṁ pamajja khattiyo,",
  "ja527:57.4_0": "raṭṭhā cavati issaro.",
  "ja527:58.1_0": "Dhammaṁ cara mahārāja,",
  "ja527:58.2_0": "mātāpitūsu khattiya;",
  "ja527:58.3_0": "Idha dhammaṁ caritvāna,",
  "ja527:58.4_0": "rāja saggaṁ gamissasi.",
  "ja527:59.1_0": "Dhammaṁ cara mahārāja,",
  "ja527:59.2_0": "puttadāresu khattiya;",
  "ja527:59.3_0": "…pe…",
  "ja527:60.1_0": "Dhammaṁ cara mahārāja,",
  "ja527:60.2_0": "mittāmaccesu khattiya;",
  "ja527:60.3_0": "…pe…",
  "ja527:61.1_0": "Dhammaṁ cara mahārāja,",
  "ja527:61.2_0": "vāhanesu balesu ca;",
  "ja527:61.3_0": "…pe…",
  "ja527:62.1_0": "Dhammaṁ cara mahārāja,",
  "ja527:62.2_0": "gāmesu nigamesu ca;",
  "ja527:62.3_0": "…pe…",
  "ja527:63.1_0": "Dhammaṁ cara mahārāja,",
  "ja527:63.2_0": "raṭṭhesu janapadesu ca;",
  "ja527:63.3_0": "…pe…",
  "ja527:64.1_0": "Dhammaṁ cara mahārāja,",
  "ja527:64.2_0": "samaṇabrāhmaṇesu ca;",
  "ja527:64.3_0": "…pe…",
  "ja527:65.1_0": "Dhammaṁ cara mahārāja,",
  "ja527:65.2_0": "migapakkhīsu khattiya;",
  "ja527:65.3_0": "…pe…",
  "ja527:66.1_0": "Dhammaṁ cara mahārāja,",
  "ja527:66.2_0": "dhammo ciṇṇo sukhāvaho;",
  "ja527:66.3_0": "Idha dhammaṁ caritvāna,",
  "ja527:66.4_0": "rāja saggaṁ gamissasi.",
  "ja527:67.1_0": "Dhammaṁ cara mahārāja,",
  "ja527:67.2_0": "saindā devā sabrahmakā;",
  "ja527:67.3_0": "Suciṇṇena divaṁ pattā,",
  "ja527:67.4_0": "mā dhammaṁ rāja pāmado”ti.",
  "ja527:68.1_0": "Ummādantījātakaṁ dutiyaṁ."
}