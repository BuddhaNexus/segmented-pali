{
  "mil7.5.3:0.1_0": "Milindapañha",
  "mil7.5.3:0.2_0": "Opammakathāpañha",
  "mil7.5.3:0.3_0": "Upacikāvagga",
  "mil7.5.3:0.4_0": "3. Undūraṅgapañha",
  "mil7.5.3:1.1_0": "“Bhante nāgasena,",
  "mil7.5.3:1.1_1": "‘undūrassa ekaṁ aṅgaṁ gahetabban’ti yaṁ vadesi,",
  "mil7.5.3:1.1_2": "katamaṁ taṁ ekaṁ aṅgaṁ gahetabban”ti?",
  "mil7.5.3:1.2_0": "“Yathā, mahārāja, undūro itocito ca vicaranto āhārūpāsīsako yeva carati;",
  "mil7.5.3:1.3_0": "evameva kho, mahārāja,",
  "mil7.5.3:1.3_1": "yoginā yogāvacarena itocito ca vicarantena yoniso manasikārūpāsīsakeneva bhavitabbaṁ.",
  "mil7.5.3:1.4_0": "Idaṁ, mahārāja, undūrassa ekaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.5.3:1.5_0": "Bhāsitampetaṁ, mahārāja, therena upasenena vaṅgantaputtena—",
  "mil7.5.3:2.1_0": "‘Dhammāsīsaṁ karitvāna,",
  "mil7.5.3:2.2_0": "viharanto vipassako;",
  "mil7.5.3:2.3_0": "Anolīno viharati,",
  "mil7.5.3:2.4_0": "upasanto sadā sato’”ti.",
  "mil7.5.3:3.1_0": "Undūraṅgapañho tatiyo."
}