{
  "thig1.6:0.1_0": "Therīgāthā 1.6",
  "thig1.6:0.2_0": "Ekakanipāta",
  "thig1.6:0.3_0": "Dhīrātherīgāthā",
  "thig1.6:1.1_0": "“Dhīre nirodhaṁ phusehi,",
  "thig1.6:1.2_0": "Saññāvūpasamaṁ sukhaṁ;",
  "thig1.6:1.3_0": "Ārādhayāhi nibbānaṁ,",
  "thig1.6:1.4_0": "Yogakkhemamanuttaran”ti.",
  "thig1.6:2.1_0": "… Dhīrā therī …."
}