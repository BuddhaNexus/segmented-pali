{
  "thig16.1:0.1_0": "Therīgāthā 16.1",
  "thig16.1:0.2_0": "Mahānipāta",
  "thig16.1:0.3_0": "Sumedhātherīgāthā",
  "thig16.1:1.1_0": "Mantāvatiyā nagare,",
  "thig16.1:1.2_0": "Rañño koñcassa aggamahesiyā;",
  "thig16.1:1.3_0": "Dhītā āsiṁ sumedhā,",
  "thig16.1:1.4_0": "Pasāditā sāsanakarehi.",
  "thig16.1:2.1_0": "Sīlavatī cittakathā,",
  "thig16.1:2.2_0": "Bahussutā buddhasāsane vinitā;",
  "thig16.1:2.3_0": "Mātāpitaro upagamma,",
  "thig16.1:2.4_0": "Bhaṇati “ubhayo nisāmetha.",
  "thig16.1:3.1_0": "Nibbānābhiratāhaṁ,",
  "thig16.1:3.2_0": "Asassataṁ bhavagataṁ yadipi dibbaṁ;",
  "thig16.1:3.3_0": "Kimaṅgaṁ pana tucchā kāmā,",
  "thig16.1:3.4_0": "Appassādā bahuvighātā.",
  "thig16.1:4.1_0": "Kāmā kaṭukā āsī,",
  "thig16.1:4.2_0": "Visūpamā yesu mucchitā bālā;",
  "thig16.1:4.3_0": "Te dīgharattaṁ niraye,",
  "thig16.1:4.4_0": "Samappitā haññante dukkhitā.",
  "thig16.1:5.1_0": "Socanti pāpakammā,",
  "thig16.1:5.2_0": "Vinipāte pāpavaddhino sadā;",
  "thig16.1:5.3_0": "Kāyena ca vācāya ca,",
  "thig16.1:5.4_0": "Manasā ca asaṁvutā bālā.",
  "thig16.1:6.1_0": "Bālā te duppaññā,",
  "thig16.1:6.2_0": "Acetanā dukkhasamudayoruddhā;",
  "thig16.1:6.3_0": "Desente ajānantā,",
  "thig16.1:6.4_0": "Na bujjhare ariyasaccāni.",
  "thig16.1:7.1_0": "Saccāni amma buddhavaradesitāni,",
  "thig16.1:7.2_0": "Te bahutarā ajānantā ye;",
  "thig16.1:7.3_0": "Abhinandanti bhavagataṁ,",
  "thig16.1:7.4_0": "Pihenti devesu upapattiṁ.",
  "thig16.1:8.1_0": "Devesupi upapatti,",
  "thig16.1:8.2_0": "Asassatā bhavagate aniccamhi;",
  "thig16.1:8.3_0": "Na ca santasanti bālā,",
  "thig16.1:8.4_0": "Punappunaṁ jāyitabbassa.",
  "thig16.1:9.1_0": "Cattāro vinipātā,",
  "thig16.1:9.2_0": "Duve ca gatiyo kathañci labbhanti;",
  "thig16.1:9.3_0": "Na ca vinipātagatānaṁ,",
  "thig16.1:9.4_0": "Pabbajjā atthi nirayesu.",
  "thig16.1:10.1_0": "Anujānātha maṁ ubhayo,",
  "thig16.1:10.2_0": "Pabbajituṁ dasabalassa pāvacane;",
  "thig16.1:10.3_0": "Appossukkā ghaṭissaṁ,",
  "thig16.1:10.4_0": "Jātimaraṇappahānāya.",
  "thig16.1:11.1_0": "Kiṁ bhavagate abhinanditena,",
  "thig16.1:11.2_0": "Kāyakalinā asārena;",
  "thig16.1:11.3_0": "Bhavataṇhāya nirodhā,",
  "thig16.1:11.4_0": "Anujānātha pabbajissāmi.",
  "thig16.1:12.1_0": "Buddhānaṁ uppādo vivajjito,",
  "thig16.1:12.2_0": "Akkhaṇo khaṇo laddho;",
  "thig16.1:12.3_0": "Sīlāni brahmacariyaṁ,",
  "thig16.1:12.4_0": "Yāvajīvaṁ na dūseyyaṁ”.",
  "thig16.1:13.1_0": "Evaṁ bhaṇati sumedhā,",
  "thig16.1:13.2_0": "Mātāpitaro “na tāva āhāraṁ;",
  "thig16.1:13.3_0": "Āharissaṁ gahaṭṭhā,",
  "thig16.1:13.4_0": "Maraṇavasaṁ gatāva hessāmi”.",
  "thig16.1:14.1_0": "Mātā dukkhitā rodati pitā ca,",
  "thig16.1:14.2_0": "Assā sabbaso samabhihato;",
  "thig16.1:14.3_0": "Ghaṭenti saññāpetuṁ,",
  "thig16.1:14.4_0": "Pāsādatale chamāpatitaṁ.",
  "thig16.1:15.1_0": "“Uṭṭhehi puttaka kiṁ soci,",
  "thig16.1:15.2_0": "Tena dinnāsi vāraṇavatimhi;",
  "thig16.1:15.3_0": "Rājā anīkaratto,",
  "thig16.1:15.4_0": "Abhirūpo tassa tvaṁ dinnā.",
  "thig16.1:16.1_0": "Aggamahesī bhavissasi,",
  "thig16.1:16.2_0": "Anikarattassa rājino bhariyā;",
  "thig16.1:16.3_0": "Sīlāni brahmacariyaṁ,",
  "thig16.1:16.4_0": "Pabbajjā dukkarā puttaka.",
  "thig16.1:17.1_0": "Rajje āṇā dhanamissariyaṁ,",
  "thig16.1:17.2_0": "Bhogā sukhā daharikāsi;",
  "thig16.1:17.3_0": "Bhuñjāhi kāmabhoge,",
  "thig16.1:17.4_0": "Vāreyyaṁ hotu te putta”.",
  "thig16.1:18.1_0": "Atha ne bhaṇati sumedhā,",
  "thig16.1:18.2_0": "“Mā edisikāni bhavagatamasāraṁ;",
  "thig16.1:18.3_0": "Pabbajjā vā hohiti,",
  "thig16.1:18.4_0": "Maraṇaṁ vā me na ceva vāreyyaṁ.",
  "thig16.1:19.1_0": "Kimiva pūtikāyamasuciṁ,",
  "thig16.1:19.2_0": "Savanagandhaṁ bhayānakaṁ kuṇapaṁ;",
  "thig16.1:19.3_0": "Abhisaṁviseyyaṁ bhastaṁ,",
  "thig16.1:19.4_0": "Asakiṁ paggharitaṁ asucipuṇṇaṁ.",
  "thig16.1:20.1_0": "Kimiva tahaṁ jānantī,",
  "thig16.1:20.2_0": "Vikūlakaṁ maṁsasoṇitupalittaṁ;",
  "thig16.1:20.3_0": "Kimikulālayaṁ sakuṇabhattaṁ,",
  "thig16.1:20.4_0": "Kaḷevaraṁ kissa diyyati.",
  "thig16.1:21.1_0": "Nibbuyhati susānaṁ,",
  "thig16.1:21.2_0": "Aciraṁ kāyo apetaviññāṇo;",
  "thig16.1:21.3_0": "Chuddho kaḷiṅgaraṁ viya,",
  "thig16.1:21.4_0": "Jigucchamānehi ñātīhi.",
  "thig16.1:22.1_0": "Chuddhūna naṁ susāne,",
  "thig16.1:22.2_0": "Parabhattaṁ nhāyanti jigucchantā;",
  "thig16.1:22.3_0": "Niyakā mātāpitaro,",
  "thig16.1:22.4_0": "Kiṁ pana sādhāraṇā janatā.",
  "thig16.1:23.1_0": "Ajjhositā asāre,",
  "thig16.1:23.2_0": "Kaḷevare aṭṭhinhārusaṅghāte;",
  "thig16.1:23.3_0": "Kheḷassuccārassava,",
  "thig16.1:23.4_0": "Paripuṇṇe pūtikāyamhi.",
  "thig16.1:24.1_0": "Yo naṁ vinibbhujitvā,",
  "thig16.1:24.2_0": "Abbhantaramassa bāhiraṁ kayirā;",
  "thig16.1:24.3_0": "Gandhassa asahamānā,",
  "thig16.1:24.4_0": "Sakāpi mātā jiguccheyya.",
  "thig16.1:25.1_0": "Khandhadhātuāyatanaṁ,",
  "thig16.1:25.2_0": "Saṅkhataṁ jātimūlakaṁ dukkhaṁ;",
  "thig16.1:25.3_0": "Yoniso anuvicinantī,",
  "thig16.1:25.4_0": "Vāreyyaṁ kissa iccheyyaṁ.",
  "thig16.1:26.1_0": "Divase divase tisatti,",
  "thig16.1:26.2_0": "Satāni navanavā pateyyuṁ kāyamhi;",
  "thig16.1:26.3_0": "Vassasatampi ca ghāto,",
  "thig16.1:26.4_0": "Seyyo dukkhassa cevaṁ khayo.",
  "thig16.1:27.1_0": "Ajjhupagacche ghātaṁ,",
  "thig16.1:27.2_0": "Yo viññāyevaṁ satthuno vacanaṁ;",
  "thig16.1:27.3_0": "‘Dīgho tesaṁ saṁsāro,",
  "thig16.1:27.4_0": "Punappunaṁ haññamānānaṁ’.",
  "thig16.1:28.1_0": "Devesu manussesu ca,",
  "thig16.1:28.2_0": "Tiracchānayoniyā asurakāye;",
  "thig16.1:28.3_0": "Petesu ca nirayesu ca,",
  "thig16.1:28.4_0": "Aparimitā dissare ghātā.",
  "thig16.1:29.1_0": "Ghātā nirayesu bahū,",
  "thig16.1:29.2_0": "Vinipātagatassa pīḷiyamānassa;",
  "thig16.1:29.3_0": "Devesupi attāṇaṁ,",
  "thig16.1:29.4_0": "Nibbānasukhā paraṁ natthi.",
  "thig16.1:30.1_0": "Pattā te nibbānaṁ,",
  "thig16.1:30.2_0": "Ye yuttā dasabalassa pāvacane;",
  "thig16.1:30.3_0": "Appossukkā ghaṭenti,",
  "thig16.1:30.4_0": "Jātimaraṇappahānāya.",
  "thig16.1:31.1_0": "Ajjeva tātabhinikkha-",
  "thig16.1:31.2_0": "Missaṁ bhogehi kiṁ asārehi;",
  "thig16.1:31.3_0": "Nibbinnā me kāmā,",
  "thig16.1:31.4_0": "Vantasamā tālavatthukatā”.",
  "thig16.1:32.1_0": "Sā cevaṁ bhaṇati pitara-",
  "thig16.1:32.2_0": "Manīkaratto ca yassa sā dinnā;",
  "thig16.1:32.3_0": "Upayāsi vāraṇavate,",
  "thig16.1:32.4_0": "Vāreyyamupaṭṭhite kāle.",
  "thig16.1:33.1_0": "Atha asitanicitamuduke,",
  "thig16.1:33.2_0": "Kese khaggena chindiya sumedhā;",
  "thig16.1:33.3_0": "Pāsādaṁ pidahitvā,",
  "thig16.1:33.4_0": "Paṭhamajjhānaṁ samāpajji.",
  "thig16.1:34.1_0": "Sā ca tahiṁ samāpannā,",
  "thig16.1:34.2_0": "Anīkaratto ca āgato nagaraṁ;",
  "thig16.1:34.3_0": "Pāsāde ca sumedhā,",
  "thig16.1:34.4_0": "Aniccasaññaṁ subhāveti.",
  "thig16.1:35.1_0": "Sā ca manasi karoti,",
  "thig16.1:35.2_0": "Anīkaratto ca āruhī turitaṁ;",
  "thig16.1:35.3_0": "Maṇikanakabhūsitaṅgo,",
  "thig16.1:35.4_0": "Katañjalī yācati sumedhaṁ.",
  "thig16.1:36.1_0": "“Rajje āṇādhanamissa-",
  "thig16.1:36.2_0": "Riyaṁ bhogā sukhā daharikāsi;",
  "thig16.1:36.3_0": "Bhuñjāhi kāmabhoge,",
  "thig16.1:36.4_0": "Kāmasukhā dullabhā loke.",
  "thig16.1:37.1_0": "Nissaṭṭhaṁ te rajjaṁ,",
  "thig16.1:37.2_0": "Bhoge bhuñjassu dehi dānāni;",
  "thig16.1:37.3_0": "Mā dummanā ahosi,",
  "thig16.1:37.4_0": "Mātāpitaro te dukkhitā”.",
  "thig16.1:38.1_0": "Taṁ taṁ bhaṇati sumedhā,",
  "thig16.1:38.2_0": "Kāmehi anatthikā vigatamohā;",
  "thig16.1:38.3_0": "“Mā kāme abhinandi,",
  "thig16.1:38.4_0": "Kāmesvādīnavaṁ passa.",
  "thig16.1:39.1_0": "Cātuddīpo rājā,",
  "thig16.1:39.2_0": "Mandhātā āsi kāmabhoginamaggo;",
  "thig16.1:39.3_0": "Atitto kālaṅkato,",
  "thig16.1:39.4_0": "Na cassa paripūritā icchā.",
  "thig16.1:40.1_0": "Satta ratanāni vasseyya,",
  "thig16.1:40.2_0": "Vuṭṭhimā dasadisā samantena;",
  "thig16.1:40.3_0": "Na catthi titti kāmānaṁ,",
  "thig16.1:40.4_0": "Atittāva maranti narā.",
  "thig16.1:41.1_0": "Asisūnūpamā kāmā,",
  "thig16.1:41.2_0": "kāmā sappasiropamā;",
  "thig16.1:41.3_0": "Ukkopamā anudahanti,",
  "thig16.1:41.4_0": "aṭṭhikaṅkala sannibhā.",
  "thig16.1:42.1_0": "Aniccā adhuvā kāmā,",
  "thig16.1:42.2_0": "bahudukkhā mahāvisā;",
  "thig16.1:42.3_0": "Ayoguḷova santatto,",
  "thig16.1:42.4_0": "aghamūlā dukhapphalā.",
  "thig16.1:43.1_0": "Rukkhapphalūpamā kāmā,",
  "thig16.1:43.2_0": "maṁsapesūpamā dukhā;",
  "thig16.1:43.3_0": "Supinopamā vañcaniyā,",
  "thig16.1:43.4_0": "kāmā yācitakūpamā.",
  "thig16.1:44.1_0": "Sattisūlūpamā kāmā,",
  "thig16.1:44.2_0": "rogo gaṇḍo aghaṁ nighaṁ;",
  "thig16.1:44.3_0": "Aṅgārakāsusadisā,",
  "thig16.1:44.4_0": "aghamūlaṁ bhayaṁ vadho.",
  "thig16.1:45.1_0": "Evaṁ bahudukkhā kāmā,",
  "thig16.1:45.2_0": "akkhātā antarāyikā;",
  "thig16.1:45.3_0": "Gacchatha na me bhavagate,",
  "thig16.1:45.4_0": "vissāso atthi attano.",
  "thig16.1:46.1_0": "Kiṁ mama paro karissati,",
  "thig16.1:46.2_0": "Attano sīsamhi ḍayhamānamhi;",
  "thig16.1:46.3_0": "Anubandhe jarāmaraṇe,",
  "thig16.1:46.4_0": "Tassa ghātāya ghaṭitabbaṁ”.",
  "thig16.1:47.1_0": "Dvāraṁ apāpuritvānahaṁ,",
  "thig16.1:47.2_0": "Mātāpitaro anīkarattañca;",
  "thig16.1:47.3_0": "Disvāna chamaṁ nisinne,",
  "thig16.1:47.4_0": "Rodante idamavocaṁ.",
  "thig16.1:48.1_0": "“Dīgho bālānaṁ saṁsāro,",
  "thig16.1:48.2_0": "Punappunañca rodataṁ;",
  "thig16.1:48.3_0": "Anamatagge pitu maraṇe,",
  "thig16.1:48.4_0": "Bhātu vadhe attano ca vadhe.",
  "thig16.1:49.1_0": "Assu thaññaṁ rudhiraṁ,",
  "thig16.1:49.2_0": "Saṁsāraṁ anamataggato saratha;",
  "thig16.1:49.3_0": "Sattānaṁ saṁsarataṁ,",
  "thig16.1:49.4_0": "Sarāhi aṭṭhīnañca sannicayaṁ.",
  "thig16.1:50.1_0": "Sara caturodadhī,",
  "thig16.1:50.2_0": "Upanīte assuthaññarudhiramhi;",
  "thig16.1:50.3_0": "Sara ekakappamaṭṭhīnaṁ,",
  "thig16.1:50.4_0": "Sañcayaṁ vipulena samaṁ.",
  "thig16.1:51.1_0": "Anamatagge saṁsarato,",
  "thig16.1:51.2_0": "Mahiṁ jambudīpamupanītaṁ;",
  "thig16.1:51.3_0": "Kolaṭṭhimattaguḷikā,",
  "thig16.1:51.4_0": "Mātā mātusveva nappahonti.",
  "thig16.1:52.1_0": "Tiṇakaṭṭhasākhāpalāsaṁ,",
  "thig16.1:52.2_0": "Upanītaṁ anamataggato sara;",
  "thig16.1:52.3_0": "Caturaṅgulikā ghaṭikā,",
  "thig16.1:52.4_0": "Pitupitusveva nappahonti.",
  "thig16.1:53.1_0": "Sara kāṇakacchapaṁ pubba-",
  "thig16.1:53.2_0": "Samudde aparato ca yugachiddaṁ;",
  "thig16.1:53.3_0": "Siraṁ tassa ca paṭimukkaṁ,",
  "thig16.1:53.4_0": "Manussalābhamhi opammaṁ.",
  "thig16.1:54.1_0": "Sara rūpaṁ pheṇapiṇḍopa-",
  "thig16.1:54.2_0": "Massa kāyakalino asārassa;",
  "thig16.1:54.3_0": "Khandhe passa anicce,",
  "thig16.1:54.4_0": "Sarāhi niraye bahuvighāte.",
  "thig16.1:55.1_0": "Sara kaṭasiṁ vaḍḍhente,",
  "thig16.1:55.2_0": "Punappunaṁ tāsu tāsu jātīsu;",
  "thig16.1:55.3_0": "Sara kumbhīlabhayāni ca,",
  "thig16.1:55.4_0": "Sarāhi cattāri saccāni.",
  "thig16.1:56.1_0": "Amatamhi vijjamāne,",
  "thig16.1:56.2_0": "Kiṁ tava pañcakaṭukena pītena;",
  "thig16.1:56.3_0": "Sabbā hi kāmaratiyo,",
  "thig16.1:56.4_0": "Kaṭukatarā pañcakaṭukena.",
  "thig16.1:57.1_0": "Amatamhi vijjamāne,",
  "thig16.1:57.2_0": "Kiṁ tava kāmehi ye pariḷāhā;",
  "thig16.1:57.3_0": "Sabbā hi kāmaratiyo,",
  "thig16.1:57.4_0": "Jalitā kuthitā kampitā santāpitā.",
  "thig16.1:58.1_0": "Asapattamhi samāne,",
  "thig16.1:58.2_0": "Kiṁ tava kāmehi ye bahusapattā;",
  "thig16.1:58.3_0": "Rājaggicoraudakappiyehi,",
  "thig16.1:58.4_0": "Sādhāraṇā kāmā bahusapattā.",
  "thig16.1:59.1_0": "Mokkhamhi vijjamāne,",
  "thig16.1:59.2_0": "Kiṁ tava kāmehi yesu vadhabandho;",
  "thig16.1:59.3_0": "Kāmesu hi asakāmā,",
  "thig16.1:59.4_0": "Vadhabandhadukhāni anubhonti.",
  "thig16.1:60.1_0": "Ādīpitā tiṇukkā,",
  "thig16.1:60.2_0": "Gaṇhantaṁ dahanti neva muñcantaṁ;",
  "thig16.1:60.3_0": "Ukkopamā hi kāmā,",
  "thig16.1:60.4_0": "Dahanti ye te na muñcanti.",
  "thig16.1:61.1_0": "Mā appakassa hetu,",
  "thig16.1:61.2_0": "Kāmasukhassa vipulaṁ jahī sukhaṁ;",
  "thig16.1:61.3_0": "Mā puthulomova baḷisaṁ,",
  "thig16.1:61.4_0": "Gilitvā pacchā vihaññasi.",
  "thig16.1:62.1_0": "Kāmaṁ kāmesu damassu,",
  "thig16.1:62.2_0": "Tāva sunakhova saṅkhalābaddho;",
  "thig16.1:62.3_0": "Kāhinti khu taṁ kāmā,",
  "thig16.1:62.4_0": "Chātā sunakhaṁva caṇḍālā.",
  "thig16.1:63.1_0": "Aparimitañca dukkhaṁ,",
  "thig16.1:63.2_0": "Bahūni ca cittadomanassāni;",
  "thig16.1:63.3_0": "Anubhohisi kāmayutto,",
  "thig16.1:63.4_0": "Paṭinissaja addhuve kāme.",
  "thig16.1:64.1_0": "Ajaramhi vijjamāne,",
  "thig16.1:64.2_0": "Kiṁ tava kāmehi yesu jarā;",
  "thig16.1:64.3_0": "Maraṇabyādhigahitā,",
  "thig16.1:64.4_0": "Sabbā sabbattha jātiyo.",
  "thig16.1:65.1_0": "Idamajaramidamamaraṁ,",
  "thig16.1:65.2_0": "Idamajarāmaraṁ padamasokaṁ;",
  "thig16.1:65.3_0": "Asapattamasambādhaṁ,",
  "thig16.1:65.4_0": "Akhalitamabhayaṁ nirupatāpaṁ.",
  "thig16.1:66.1_0": "Adhigatamidaṁ bahūhi,",
  "thig16.1:66.2_0": "Amataṁ ajjāpi ca labhanīyamidaṁ;",
  "thig16.1:66.3_0": "Yo yoniso payuñjati,",
  "thig16.1:66.4_0": "Na ca sakkā aghaṭamānena”.",
  "thig16.1:67.1_0": "Evaṁ bhaṇati sumedhā,",
  "thig16.1:67.2_0": "Saṅkhāragate ratiṁ alabhamānā;",
  "thig16.1:67.3_0": "Anunentī anikarattaṁ,",
  "thig16.1:67.4_0": "Kese ca chamaṁ khipi sumedhā.",
  "thig16.1:68.1_0": "Uṭṭhāya anikaratto,",
  "thig16.1:68.2_0": "Pañjaliko yācatassā pitaraṁ so;",
  "thig16.1:68.3_0": "“Vissajjetha sumedhaṁ,",
  "thig16.1:68.4_0": "Pabbajituṁ vimokkhasaccadassā”.",
  "thig16.1:69.1_0": "Vissajjitā mātāpitūhi,",
  "thig16.1:69.2_0": "pabbaji sokabhayabhītā;",
  "thig16.1:69.3_0": "Cha abhiññā sacchikatā,",
  "thig16.1:69.4_0": "aggaphalaṁ sikkhamānāya.",
  "thig16.1:70.1_0": "Acchariyamabbhutaṁ taṁ,",
  "thig16.1:70.2_0": "Nibbānaṁ āsi rājakaññāya;",
  "thig16.1:70.3_0": "Pubbenivāsacaritaṁ,",
  "thig16.1:70.4_0": "Yathā byākari pacchime kāle.",
  "thig16.1:71.1_0": "“Bhagavati koṇāgamane,",
  "thig16.1:71.2_0": "Saṅghārāmamhi navanivesamhi;",
  "thig16.1:71.3_0": "Sakhiyo tisso janiyo,",
  "thig16.1:71.4_0": "Vihāradānaṁ adāsimha.",
  "thig16.1:72.1_0": "Dasakkhattuṁ satakkhattuṁ,",
  "thig16.1:72.2_0": "Dasasatakkhattuṁ satāni ca satakkhattuṁ;",
  "thig16.1:72.3_0": "Devesu uppajjimha,",
  "thig16.1:72.4_0": "Ko pana vādo manussesu.",
  "thig16.1:73.1_0": "Devesu mahiddhikā ahumha,",
  "thig16.1:73.2_0": "Mānusakamhi ko pana vādo;",
  "thig16.1:73.3_0": "Sattaratanassa mahesī,",
  "thig16.1:73.4_0": "Itthiratanaṁ ahaṁ āsiṁ.",
  "thig16.1:74.1_0": "So hetu so pabhavo,",
  "thig16.1:74.2_0": "Taṁ mūlaṁ sāva sāsane khantī;",
  "thig16.1:74.3_0": "Taṁ paṭhamasamodhānaṁ,",
  "thig16.1:74.4_0": "Taṁ dhammaratāya nibbānaṁ.",
  "thig16.1:75.1_0": "Evaṁ karonti ye sadda-",
  "thig16.1:75.2_0": "Hanti vacanaṁ anomapaññassa;",
  "thig16.1:75.3_0": "Nibbindanti bhavagate,",
  "thig16.1:75.4_0": "Nibbinditvā virajjantī”ti.",
  "thig16.1:76.1_0": "Itthaṁ sudaṁ sumedhā therī gāthāyo abhāsitthāti.",
  "thig16.1:76.2_0": "Mahānipāto niṭṭhito.",
  "thig16.1:77.0_0": "Samattā therīgāthāyo",
  "thig16.1:77.1_0": "Gāthāsatāni cattāri,",
  "thig16.1:77.2_0": "asīti puna cuddasa;",
  "thig16.1:77.3_0": "Theriyekuttarasatā,",
  "thig16.1:77.4_0": "sabbā tā āsavakkhayāti.",
  "thig16.1:77.5_0": "Therīgāthāpāḷi niṭṭhitā."
}