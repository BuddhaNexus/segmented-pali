{
  "thag6.10:0.1_0": "Theragāthā 6.10",
  "thag6.10:0.2_0": "Chakkanipāta",
  "thag6.10:0.3_0": "Paṭhamavagga",
  "thag6.10:0.4_0": "Sumanattheragāthā",
  "thag6.10:1.1_0": "“Yadā navo pabbajito,",
  "thag6.10:1.2_0": "jātiyā sattavassiko;",
  "thag6.10:1.3_0": "Iddhiyā abhibhotvāna,",
  "thag6.10:1.4_0": "pannagindaṁ mahiddhikaṁ.",
  "thag6.10:2.1_0": "Upajjhāyassa udakaṁ,",
  "thag6.10:2.2_0": "anotattā mahāsarā;",
  "thag6.10:2.3_0": "Āharāmi tato disvā,",
  "thag6.10:2.4_0": "maṁ satthā etadabravi”.",
  "thag6.10:3.1_0": "“Sāriputta imaṁ passa,",
  "thag6.10:3.2_0": "āgacchantaṁ kumārakaṁ;",
  "thag6.10:3.3_0": "Udakakumbhamādāya,",
  "thag6.10:3.4_0": "ajjhattaṁ susamāhitaṁ.",
  "thag6.10:4.1_0": "Pāsādikena vattena,",
  "thag6.10:4.2_0": "kalyāṇairiyāpatho;",
  "thag6.10:4.3_0": "Sāmaṇeronuruddhassa,",
  "thag6.10:4.4_0": "iddhiyā ca visārado.",
  "thag6.10:5.1_0": "Ājānīyena ājañño,",
  "thag6.10:5.2_0": "sādhunā sādhukārito;",
  "thag6.10:5.3_0": "Vinīto anuruddhena,",
  "thag6.10:5.4_0": "katakiccena sikkhito.",
  "thag6.10:6.1_0": "So patvā paramaṁ santiṁ,",
  "thag6.10:6.2_0": "sacchikatvā akuppataṁ;",
  "thag6.10:6.3_0": "Sāmaṇero sa sumano,",
  "thag6.10:6.4_0": "mā maṁ jaññāti icchatī”ti.",
  "thag6.10:6.5_0": "… Sumano thero …."
}