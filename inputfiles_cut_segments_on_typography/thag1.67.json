{
  "thag1.67:0.1_0": "Theragāthā 1.67",
  "thag1.67:0.2_0": "Ekakanipāta",
  "thag1.67:0.3_0": "Sattamavagga",
  "thag1.67:0.4_0": "Ekadhammasavanīyattheragāthā",
  "thag1.67:1.1_0": "“Kilesā jhāpitā mayhaṁ,",
  "thag1.67:1.2_0": "bhavā sabbe samūhatā;",
  "thag1.67:1.3_0": "Vikkhīṇo jātisaṁsāro,",
  "thag1.67:1.4_0": "natthi dāni punabbhavo”ti.",
  "thag1.67:1.5_0": "… Ekadhammasavanīyo thero …."
}