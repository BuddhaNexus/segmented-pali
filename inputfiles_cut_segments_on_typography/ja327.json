{
  "ja327:0.1_0": "Jātaka",
  "ja327:0.2_0": "Catukkanipāta",
  "ja327:0.3_0": "Kuṭidūsakavagga",
  "ja327:0.4_0": "7. Kākavatījātaka",
  "ja327:1.1_0": "“Vāti cāyaṁ tato gandho,",
  "ja327:1.2_0": "yattha me vasatī piyā;",
  "ja327:1.3_0": "Dūre ito hi kākavatī,",
  "ja327:1.4_0": "yattha me nirato mano”.",
  "ja327:2.1_0": "“Kathaṁ samuddamatarī,",
  "ja327:2.2_0": "kathaṁ atari kepukaṁ;",
  "ja327:2.3_0": "Kathaṁ satta samuddāni,",
  "ja327:2.4_0": "kathaṁ simbalimāruhi”.",
  "ja327:3.1_0": "“Tayā samuddamatariṁ,",
  "ja327:3.2_0": "tayā atari kepukaṁ;",
  "ja327:3.3_0": "Tayā satta samuddāni,",
  "ja327:3.4_0": "tayā simbalimāruhiṁ”.",
  "ja327:4.1_0": "“Dhiratthumaṁ mahākāyaṁ,",
  "ja327:4.2_0": "dhiratthumaṁ acetanaṁ;",
  "ja327:4.3_0": "Yattha jāyāyahaṁ jāraṁ,",
  "ja327:4.4_0": "āvahāmi vahāmi cā”ti.",
  "ja327:5.1_0": "Kākavatījātakaṁ sattamaṁ."
}