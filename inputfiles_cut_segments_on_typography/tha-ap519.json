{
  "tha-ap519:0.1_0": "Therāpadāna",
  "tha-ap519:0.2_0": "Phaladāyakavagga",
  "tha-ap519:0.3_0": "7. Kuṭajapupphiyattheraapadāna",
  "tha-ap519:1.1_0": "“Himavantassāvidūre,",
  "tha-ap519:1.2_0": "vasalo nāma pabbato;",
  "tha-ap519:1.3_0": "Buddho sudassano nāma,",
  "tha-ap519:1.4_0": "vasate pabbatantare.",
  "tha-ap519:2.1_0": "Pupphaṁ hemavantaṁ gayha,",
  "tha-ap519:2.2_0": "vehāsaṁ agamāsahaṁ;",
  "tha-ap519:2.3_0": "Tatthaddasāsiṁ sambuddhaṁ,",
  "tha-ap519:2.4_0": "oghatiṇṇamanāsavaṁ.",
  "tha-ap519:3.1_0": "Pupphaṁ kuṭajamādāya,",
  "tha-ap519:3.2_0": "sire katvāna añjaliṁ;",
  "tha-ap519:3.3_0": "Buddhassa abhiropesiṁ,",
  "tha-ap519:3.4_0": "sayambhussa mahesino.",
  "tha-ap519:4.1_0": "Ekatiṁse ito kappe,",
  "tha-ap519:4.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap519:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap519:4.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap519:5.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap519:5.2_0": "…pe…",
  "tha-ap519:5.3_0": "viharāmi anāsavo.",
  "tha-ap519:6.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap519:6.2_0": "…pe…",
  "tha-ap519:6.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap519:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap519:7.2_0": "…pe…",
  "tha-ap519:7.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap519:8.1_0": "Itthaṁ sudaṁ āyasmā kuṭajapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap519:9.1_0": "Kuṭajapupphiyattherassāpadānaṁ sattamaṁ."
}