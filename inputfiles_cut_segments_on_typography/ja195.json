{
  "ja195:0.1_0": "Jātaka",
  "ja195:0.2_0": "Dukanipāta",
  "ja195:0.3_0": "Ruhakavagga",
  "ja195:0.4_0": "5. Pabbatūpattharajātaka",
  "ja195:1.1_0": "“Pabbatūpatthare ramme,",
  "ja195:1.2_0": "jātā pokkharaṇī sivā;",
  "ja195:1.3_0": "Taṁ siṅgālo apāpāyi,",
  "ja195:1.4_0": "jānaṁ sīhena rakkhitaṁ”.",
  "ja195:2.1_0": "“Pivanti ce mahārāja,",
  "ja195:2.2_0": "sāpadāni mahānadiṁ;",
  "ja195:2.3_0": "Na tena anadī hoti,",
  "ja195:2.4_0": "khamassu yadi te piyā”ti.",
  "ja195:3.1_0": "Pabbatūpattharajātakaṁ pañcamaṁ."
}