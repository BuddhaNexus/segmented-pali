{
  "ja328:0.1_0": "Jātaka",
  "ja328:0.2_0": "Catukkanipāta",
  "ja328:0.3_0": "Kuṭidūsakavagga",
  "ja328:0.4_0": "8. Ananusociyajātaka",
  "ja328:1.1_0": "“Bahūnaṁ vijjatī bhotī,",
  "ja328:1.2_0": "tehi me kiṁ bhavissati;",
  "ja328:1.3_0": "Tasmā etaṁ na socāmi,",
  "ja328:1.4_0": "piyaṁ sammillahāsiniṁ.",
  "ja328:2.1_0": "Taṁ tañce anusoceyya,",
  "ja328:2.2_0": "yaṁ yaṁ tassa na vijjati;",
  "ja328:2.3_0": "Attānamanusoceyya,",
  "ja328:2.4_0": "sadā maccuvasaṁ pataṁ.",
  "ja328:3.1_0": "Na heva ṭhitaṁ nāsīnaṁ,",
  "ja328:3.2_0": "Na sayānaṁ na paddhaguṁ;",
  "ja328:3.3_0": "Yāva byāti nimisati,",
  "ja328:3.4_0": "Tatrāpi rasatī vayo.",
  "ja328:4.1_0": "Tatthattani vatappaddhe,",
  "ja328:4.2_0": "vinābhāve asaṁsaye;",
  "ja328:4.3_0": "Bhūtaṁ sesaṁ dayitabbaṁ,",
  "ja328:4.4_0": "vītaṁ ananusociyan”ti.",
  "ja328:5.1_0": "Ananusociyajātakaṁ aṭṭhamaṁ."
}