{
  "tha-ap192:0.1_0": "Therāpadāna",
  "tha-ap192:0.2_0": "Kuṭajapupphiyavagga",
  "tha-ap192:0.3_0": "10. Tivaṇṭipupphiyattheraapadāna",
  "tha-ap192:1.1_0": "“Abhibhūtaṁ panijjhanti,",
  "tha-ap192:1.2_0": "Sabbe saṅgamma te mamaṁ;",
  "tha-ap192:1.3_0": "Tesaṁ nijjhāyamānānaṁ,",
  "tha-ap192:1.4_0": "Pariḷāho ajāyatha.",
  "tha-ap192:2.1_0": "Sunando nāma nāmena,",
  "tha-ap192:2.2_0": "buddhassa sāvako tadā;",
  "tha-ap192:2.3_0": "Dhammadassissa munino,",
  "tha-ap192:2.4_0": "āgacchi mama santikaṁ.",
  "tha-ap192:3.1_0": "Ye me baddhacarā āsuṁ,",
  "tha-ap192:3.2_0": "te me pupphaṁ aduṁ tadā;",
  "tha-ap192:3.3_0": "Tāhaṁ pupphaṁ gahetvāna,",
  "tha-ap192:3.4_0": "sāvake abhiropayiṁ.",
  "tha-ap192:4.1_0": "Sohaṁ kālaṅkato tattha,",
  "tha-ap192:4.2_0": "punāpi upapajjahaṁ;",
  "tha-ap192:4.3_0": "Aṭṭhārase kappasate,",
  "tha-ap192:4.4_0": "vinipātaṁ na gacchahaṁ.",
  "tha-ap192:5.1_0": "Teraseto kappasate,",
  "tha-ap192:5.2_0": "aṭṭhāsuṁ dhūmaketuno;",
  "tha-ap192:5.3_0": "Sattaratanasampannā,",
  "tha-ap192:5.4_0": "cakkavattī mahabbalā.",
  "tha-ap192:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap192:6.2_0": "…pe…",
  "tha-ap192:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap192:7.1_0": "Itthaṁ sudaṁ āyasmā tivaṇṭipupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap192:8.1_0": "Tivaṇṭipupphiyattherassāpadānaṁ dasamaṁ.",
  "tha-ap192:9.1_0": "Kuṭajapupphiyavaggo ekūnavīsatimo.",
  "tha-ap192:10.1_0": "Tassuddānaṁ",
  "tha-ap192:11.1_0": "Kuṭajo bandhujīvī ca,",
  "tha-ap192:11.2_0": "koṭumbarikahatthiyo;",
  "tha-ap192:11.3_0": "Isimuggo ca bodhi ca,",
  "tha-ap192:11.4_0": "ekacintī tikaṇṇiko;",
  "tha-ap192:11.5_0": "Ekacārī tivaṇṭi ca,",
  "tha-ap192:11.6_0": "gāthā dvāsaṭṭhi kittitāti."
}