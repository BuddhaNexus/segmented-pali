{
  "ja311:0.1_0": "Jātaka",
  "ja311:0.2_0": "Catukkanipāta",
  "ja311:0.3_0": "Pucimandavagga",
  "ja311:0.4_0": "1. Pucimandajātaka",
  "ja311:1.1_0": "“Uṭṭhehi cora kiṁ sesi,",
  "ja311:1.2_0": "ko attho supanena te;",
  "ja311:1.3_0": "Mā taṁ gahesuṁ rājāno,",
  "ja311:1.4_0": "gāme kibbisakārakaṁ”.",
  "ja311:2.1_0": "“Yannu coraṁ gahessanti,",
  "ja311:2.2_0": "gāme kibbisakārakaṁ;",
  "ja311:2.3_0": "Kiṁ tattha pucimandassa,",
  "ja311:2.4_0": "vane jātassa tiṭṭhato”.",
  "ja311:3.1_0": "“Na tvaṁ assattha jānāsi,",
  "ja311:3.2_0": "mama corassa cantaraṁ;",
  "ja311:3.3_0": "Coraṁ gahetvā rājāno,",
  "ja311:3.4_0": "gāme kibbisakārakaṁ;",
  "ja311:3.5_0": "Appenti nimbasūlasmiṁ,",
  "ja311:3.6_0": "tasmiṁ me saṅkate mano”.",
  "ja311:4.1_0": "“Saṅkeyya saṅkitabbāni,",
  "ja311:4.2_0": "rakkheyyānāgataṁ bhayaṁ;",
  "ja311:4.3_0": "Anāgatabhayā dhīro,",
  "ja311:4.4_0": "ubho loke avekkhatī”ti.",
  "ja311:5.1_0": "Pucimandajātakaṁ paṭhamaṁ."
}