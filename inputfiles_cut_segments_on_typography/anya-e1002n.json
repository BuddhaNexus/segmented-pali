{
  "anya-e1002n:6_0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1002n:7_0": "Nītimañjarī",
  "anya-e1002n:8_0": "1.",
  "anya-e1002n:9_0": "Kulakkhaye vinassanti,",
  "anya-e1002n:10_0": "Kuladhammā sanantanā;",
  "anya-e1002n:11_0": "Dhamme naṭṭhe kulaṃ sabbaṃ,",
  "anya-e1002n:12_0": "Adhammo abhibhū khalaṃ.",
  "anya-e1002n:13_0": "2.",
  "anya-e1002n:14_0": "Adhammābhibhavā dantā,",
  "anya-e1002n:15_0": "Padussanti kulitthiyo;",
  "anya-e1002n:16_0": "Thīsu duṭṭhā sva dhammena,",
  "anya-e1002n:17_0": "Jāyate vaṇṇasaṅkaro.",
  "anya-e1002n:18_0": "3.",
  "anya-e1002n:19_0": "Piyaṃ bhāse guṇaggāho,",
  "anya-e1002n:20_0": "Sūro siyā vikantano;",
  "anya-e1002n:21_0": "Dātā candasamā nārī,",
  "anya-e1002n:22_0": "Diṭṭhaṃ diṭṭhaṃ nahāsaye.",
  "anya-e1002n:23_0": "4.",
  "anya-e1002n:24_0": "Kutotthi kumitte saccaṃ,",
  "anya-e1002n:25_0": "Kudāre rativaḍḍhanaṃ;",
  "anya-e1002n:26_0": "Kudesamhi mano rammaṃ,",
  "anya-e1002n:27_0": "Kurāje bhogasampadaṃ.",
  "anya-e1002n:28_0": "Saṅketeva amittasmiṃ,",
  "anya-e1002n:29_0": "Mittasmiṃ pi navissase;",
  "anya-e1002n:30_0": "Abhayā bhaya muppannaṃ,",
  "anya-e1002n:31_0": "Api mūlāni kantati.",
  "anya-e1002n:32_0": "Adiṭṭhova paro seyyo,",
  "anya-e1002n:33_0": "Dummitto no vissāsiko.",
  "anya-e1002n:34_0": "Aggihomaphalaṃ vedo,",
  "anya-e1002n:35_0": "Satthaṃsīlaphalaṃ mataṃ;",
  "anya-e1002n:36_0": "Ratiputtaphalaṃ nārī,",
  "anya-e1002n:37_0": "Dānabhuttiphalaṃ dhanaṃ.",
  "anya-e1002n:38_0": "Asaccaṃ sāhasaṃ māyā,",
  "anya-e1002n:39_0": "Mūḷhatta ma tilobhatā;",
  "anya-e1002n:40_0": "Asocaṃ niddayattañca,",
  "anya-e1002n:41_0": "Thīnaṃ dosā sabhāvajā.",
  "anya-e1002n:42_0": "Jāyāya bhattuno bhāro,",
  "anya-e1002n:43_0": "Sissena guruno kato;",
  "anya-e1002n:44_0": "Amaccakehi rājassa,",
  "anya-e1002n:45_0": "Pitarānaṃ nijenaca.",
  "anya-e1002n:46_0": "5.",
  "anya-e1002n:47_0": "Uyyamena hi sijjhanti,",
  "anya-e1002n:48_0": "Kammāni na manorathā;",
  "anya-e1002n:49_0": "Na hi suttassa sīhassa,",
  "anya-e1002n:50_0": "Pavīsanti mukhe migā.",
  "anya-e1002n:51_0": "Atisītaṃ atiuṇhaṃ,",
  "anya-e1002n:52_0": "Atisāyamidaṃ ahu;",
  "anya-e1002n:53_0": "Iti visaṭṭhakammante,",
  "anya-e1002n:54_0": "Khaṇā accenti māṇave.",
  "anya-e1002n:55_0": "Ādānassa padānassa,",
  "anya-e1002n:56_0": "Kattabbassa ca kammuno;",
  "anya-e1002n:57_0": "Khippaṃ akayyamānassa,",
  "anya-e1002n:58_0": "Kāle pivati sampadaṃ.",
  "anya-e1002n:59_0": "Nādabbe nihitā kāci,",
  "anya-e1002n:60_0": "Kriyā phalavatī bhave;",
  "anya-e1002n:61_0": "Nabyāpārasatenāpi,",
  "anya-e1002n:62_0": "Sukova pāṭhate bako.",
  "anya-e1002n:63_0": "Yo dandhakāle tarati,",
  "anya-e1002n:64_0": "Taraṇīye ca dandhaye;",
  "anya-e1002n:65_0": "Sukkhapaṇṇaṃ va akkamma,",
  "anya-e1002n:66_0": "Atthaṃ bhañjati attano.",
  "anya-e1002n:67_0": "6.",
  "anya-e1002n:68_0": "Yaṃ dadāti yaṃ bhuñjati,",
  "anya-e1002n:69_0": "Tadeva dhanino dhanaṃ;",
  "anya-e1002n:70_0": "Aññe matassa kīḷanti,",
  "anya-e1002n:71_0": "Dārehipi dhanehipi.",
  "anya-e1002n:72_0": "Dānopabhogahīnena,",
  "anya-e1002n:73_0": "Dhanena dhanino sukhaṃ;",
  "anya-e1002n:74_0": "Ko viseso daliddassa,",
  "anya-e1002n:75_0": "Adhikaṃ dhanarakkhaṇaṃ.",
  "anya-e1002n:76_0": "Nijasokhyaṃ nirundhanto,",
  "anya-e1002n:77_0": "Nīcabhogo mitampaco;",
  "anya-e1002n:78_0": "Dhanaṃ sañcayate yo so,",
  "anya-e1002n:79_0": "Parabhāravaho pasu.",
  "anya-e1002n:80_0": "Yaṃ ussukā saṅkharonti,",
  "anya-e1002n:81_0": "Alakkhikā bahuṃ dhanaṃ;",
  "anya-e1002n:82_0": "Sippavanto asippāvā,",
  "anya-e1002n:83_0": "Lakkhi vā tāni bhuñjati.",
  "anya-e1002n:84_0": "7.",
  "anya-e1002n:85_0": "Sampatyaṃ mahataṃ cittaṃ,",
  "anya-e1002n:86_0": "Bhave uppale komalaṃ;",
  "anya-e1002n:87_0": "Vipatyaṃca mahāsela,",
  "anya-e1002n:88_0": "Silāsaṅghātakakkasaṃ.",
  "anya-e1002n:89_0": "8.",
  "anya-e1002n:90_0": "Asambhabyaguṇaṃ thutvā,",
  "anya-e1002n:91_0": "Khedo mudhāva jāyate;",
  "anya-e1002n:92_0": "Avhāyaṃ canda mu llokya,",
  "anya-e1002n:93_0": "Nacandota mu pāgamī.",
  "anya-e1002n:94_0": "9.",
  "anya-e1002n:95_0": "Saccaṃ mukhamhi dhāreyya,",
  "anya-e1002n:96_0": "Kaṇṇe sutaṃ bhuje jayaṃ;",
  "anya-e1002n:97_0": "Hadayamhi khamaṃ vīraṃ,",
  "anya-e1002n:98_0": "Lokādāsaṃca locane.",
  "anya-e1002n:99_0": "Saddamattaṃ naphandeyya,",
  "anya-e1002n:100_0": "Aññatvā saddakāraṇaṃ;",
  "anya-e1002n:101_0": "Saddahetuṃ pariññāya,",
  "anya-e1002n:102_0": "Pamodo vā bhayo tathā.",
  "anya-e1002n:103_0": "Sabbasuta ma dhīyeyya,",
  "anya-e1002n:104_0": "Hīnamukkaṭṭhamajjhimaṃ.",
  "anya-e1002n:105_0": "10.",
  "anya-e1002n:106_0": "Dunnāriyā kulaṃ suddhaṃ,",
  "anya-e1002n:107_0": "Putto nassati lālanā;",
  "anya-e1002n:108_0": "Samiddhi anayā bandhu,",
  "anya-e1002n:109_0": "Pavāsā madanā hirī.",
  "anya-e1002n:110_0": "Lālaye pañcavassāni,",
  "anya-e1002n:111_0": "Dasavassāni tālaye;",
  "anya-e1002n:112_0": "Pattetu soḷasevasse,",
  "anya-e1002n:113_0": "Puttaṃ mittaṃva ācare.",
  "anya-e1002n:114_0": "Lālane bahavo dosā,",
  "anya-e1002n:115_0": "Lālane bahavo guṇā.",
  "anya-e1002n:116_0": "Pāpā nivārayati yojayate hitāya,",
  "anya-e1002n:117_0": "Guyhāni gūhati guṇaṃ pakaṭīkaroti;",
  "anya-e1002n:118_0": "Āpattikañca najahāti dadāti kāle,",
  "anya-e1002n:119_0": "Sammitta lakkhaṇamidaṃ pavadanti santo.",
  "anya-e1002n:120_0": "11.",
  "anya-e1002n:121_0": "Dujjano jīyate yutyā,",
  "anya-e1002n:122_0": "Niggahena nadhīmatā;",
  "anya-e1002n:123_0": "Nipātyate mahārukkho,",
  "anya-e1002n:124_0": "Tassamīpa khatikkhayā.",
  "anya-e1002n:125_0": "Vane migāca luddhānaṃ,",
  "anya-e1002n:126_0": "Dujjanānañca sajjanā;",
  "anya-e1002n:127_0": "Akāraṇaverī honti,",
  "anya-e1002n:128_0": "Tiṇabhakkhā supesalā.",
  "anya-e1002n:129_0": "Pādalaggaṃ karaṭṭhena,",
  "anya-e1002n:130_0": "Kaṇḍakeneva kaṇḍakaṃ.",
  "anya-e1002n:131_0": "Bālaṃ napasse nasuṇe,",
  "anya-e1002n:132_0": "Nacabālena saṃvase;",
  "anya-e1002n:133_0": "Bālenāllāpasallāpaṃ,",
  "anya-e1002n:134_0": "Nakare nacarocaye.",
  "anya-e1002n:135_0": "12.",
  "anya-e1002n:136_0": "Upa kattuṃ yathā khuddo,",
  "anya-e1002n:137_0": "Samattho natathāmahā;",
  "anya-e1002n:138_0": "Kūpo hi hanti pipāsaṃ,",
  "anya-e1002n:139_0": "Natu pāyo mahambudhi.",
  "anya-e1002n:140_0": "13.",
  "anya-e1002n:141_0": "Ādānassa padānassa,",
  "anya-e1002n:142_0": "Kattabbassaca kammuno;",
  "anya-e1002n:143_0": "Khippaṃ akaramānassa,",
  "anya-e1002n:144_0": "Kālo bhakkhati taṃ rasaṃ.",
  "anya-e1002n:145_0": "Nakkhattaṃ paṭimānentaṃ,",
  "anya-e1002n:146_0": "Attho bālaṃ upajjhagā;",
  "anya-e1002n:147_0": "Attho atthassa nakkhattaṃ,",
  "anya-e1002n:148_0": "Kiṃ karissanti tārakā.",
  "anya-e1002n:149_0": "Ajarāmarova pañño,",
  "anya-e1002n:150_0": "Vijjamatthañca cintaye;",
  "anya-e1002n:151_0": "Gahitoviya kesesu,",
  "anya-e1002n:152_0": "Maccunā dhammamācare.",
  "anya-e1002n:153_0": "14.",
  "anya-e1002n:154_0": "Vajjā gurūca mantīca,",
  "anya-e1002n:155_0": "Tayo raṭṭhābhisaṅkhatā;",
  "anya-e1002n:156_0": "Jīvīta dakkha kosānaṃ,",
  "anya-e1002n:157_0": "Vaḍḍhanā nāsanāca te.",
  "anya-e1002n:158_0": "15.",
  "anya-e1002n:159_0": "Thirena kammaṃ vaḍḍhati,",
  "anya-e1002n:160_0": "Athirena turena no;",
  "anya-e1002n:161_0": "Phalanti samaye rukkhā,",
  "anya-e1002n:162_0": "Sittāpi bahuvārinā.",
  "anya-e1002n:163_0": "Vāyāmetheva puriso,",
  "anya-e1002n:164_0": "Nanibbindeyya paṇḍito.",
  "anya-e1002n:165_0": "Payatano tādiso neva,",
  "anya-e1002n:166_0": "Kayyo yena phalaṃ nahi;",
  "anya-e1002n:167_0": "Selagge kūpakhaṇanā,",
  "anya-e1002n:168_0": "Kathaṃ toyasamāgamo.",
  "anya-e1002n:169_0": "Ñāṇaṅkusena sammaggaṃ,",
  "anya-e1002n:170_0": "Niyyatyussāhakuñjaro.",
  "anya-e1002n:171_0": "Asamekkhitakammantaṃ,",
  "anya-e1002n:172_0": "Turitābhi nipātinaṃ;",
  "anya-e1002n:173_0": "Tānikammāni tappenti,",
  "anya-e1002n:174_0": "Uṇhaṃ va jjhohitaṃ mukhe.",
  "anya-e1002n:175_0": "16.",
  "anya-e1002n:176_0": "Chaddosā puriseneha,",
  "anya-e1002n:177_0": "Hātabbā bhūtimicchantā;",
  "anya-e1002n:178_0": "Niddā majjaṃ bhayaṃ kodho,",
  "anya-e1002n:179_0": "Ālasyaṃ dīghasuttatā.",
  "anya-e1002n:180_0": "Na divā suppasīlena,",
  "anya-e1002n:181_0": "Rattimuṭṭhānadessinā;",
  "anya-e1002n:182_0": "Niccasoṇḍena mattena,",
  "anya-e1002n:183_0": "Sakkā āvasituṃ gharaṃ.",
  "anya-e1002n:184_0": "Abhetabbamhi bhāyanti,",
  "anya-e1002n:185_0": "Bhāyitabbe nabhāyare;",
  "anya-e1002n:186_0": "Bhayābhaya vimuḷhā te,",
  "anya-e1002n:187_0": "Jimhānugā ujuñjahā.",
  "anya-e1002n:188_0": "Yassa manussabhūtassa,",
  "anya-e1002n:189_0": "Natthi bhogāca sippakaṃ;",
  "anya-e1002n:190_0": "Kiṃ phalaṃ tassa mānussaṃ,",
  "anya-e1002n:191_0": "Dvipādaṭṭho hi so migo.",
  "anya-e1002n:192_0": "17.",
  "anya-e1002n:193_0": "Nānopāyova kattabbo,",
  "anya-e1002n:194_0": "Sace bhaveyya attano;",
  "anya-e1002n:195_0": "Atthasiddhi yathākāmaṃ,",
  "anya-e1002n:196_0": "Upāyo hi hitañjaso.",
  "anya-e1002n:197_0": "Lañjadānabālisena,",
  "anya-e1002n:198_0": "Kūṭaḍḍakāradhīvarā;",
  "anya-e1002n:199_0": "Vinicchayamahāmacchaṃ,",
  "anya-e1002n:200_0": "Oṭṭenti lobhasāgare.",
  "anya-e1002n:201_0": "Yassete caturo dhammā,",
  "anya-e1002n:202_0": "Vānarinda yathātava;",
  "anya-e1002n:203_0": "Saccaṃ dhammo dhīti cāgo,",
  "anya-e1002n:204_0": "Diṭṭhaṃ so ativattati.",
  "anya-e1002n:205_0": "18.",
  "anya-e1002n:206_0": "Vidvāca ratanaṃ nārī,",
  "anya-e1002n:207_0": "Vīṇā sātthaṃ giraṃmahī;",
  "anya-e1002n:208_0": "Guṇavisesa māgamma,",
  "anya-e1002n:209_0": "Guṇāni aguṇānica.",
  "anya-e1002n:210_0": "Dhanavā balavā loke,",
  "anya-e1002n:211_0": "Dhanā bhavati paṇḍito.",
  "anya-e1002n:212_0": "Sumane nissito kīṭo,",
  "anya-e1002n:213_0": "Nigguṇo hīnako sayaṃ;",
  "anya-e1002n:214_0": "Taṃ pupphehi maṇḍentānaṃ,",
  "anya-e1002n:215_0": "Raññaṃ siropi rohati.",
  "anya-e1002n:216_0": "Alakkhikehi sañcītā,",
  "anya-e1002n:217_0": "Dhanabhogāca cintitā;",
  "anya-e1002n:218_0": "Lakkhikassa bhavantete,",
  "anya-e1002n:219_0": "Lakkhivā suṭṭhubhuñjati.",
  "anya-e1002n:220_0": "Khattiyo seṭṭho jane tasmiṃ,",
  "anya-e1002n:221_0": "Yo gottapaṭisārino;",
  "anya-e1002n:222_0": "Vijjācaraṇasampanno,",
  "anya-e1002n:223_0": "So seṭṭho devamānuse.",
  "anya-e1002n:224_0": "Visāpi amataṃ gaṇhe,",
  "anya-e1002n:225_0": "Gūthato maṇimuttamaṃ;",
  "anya-e1002n:226_0": "Kaṇṭakapādapā pupphaṃ,",
  "anya-e1002n:227_0": "Thirataṃ dukkulā varaṃ.",
  "anya-e1002n:228_0": "Dhanissarādiguṇommi -",
  "anya-e1002n:229_0": "Vegena vāhitā pajā.",
  "anya-e1002n:230_0": "19.",
  "anya-e1002n:231_0": "Yassa tthi satataṃ mettā,",
  "anya-e1002n:232_0": "Sabbalokasuvallabhā;",
  "anya-e1002n:233_0": "Kūpāyate samuddopi,",
  "anya-e1002n:234_0": "Aggi tassa jalāyate.",
  "anya-e1002n:235_0": "20.",
  "anya-e1002n:236_0": "Sakkharāyati merūpi,",
  "anya-e1002n:237_0": "Visabhakkho sudhāyate;",
  "anya-e1002n:238_0": "Sasāyate migarāja,",
  "anya-e1002n:239_0": "Byālo mālāguṇāyate;",
  "anya-e1002n:240_0": "Dolāyate chamācālo,",
  "anya-e1002n:241_0": "Nānāvudhā tiṇāyare.",
  "anya-e1002n:242_0": "21.",
  "anya-e1002n:243_0": "Sameva sati ussāhe,",
  "anya-e1002n:244_0": "Sukhavāho hitaṅkaro;",
  "anya-e1002n:245_0": "Ūne-dhike tathā nohi,",
  "anya-e1002n:246_0": "Majjhago sādhu sabbadā.",
  "anya-e1002n:247_0": "Sādhu kho paṇḍitonāma,",
  "anya-e1002n:248_0": "Natveva atipaṇḍito."
}