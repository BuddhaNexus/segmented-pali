{
  "thig2.10:0.1_0": "Therīgāthā 2.10",
  "thig2.10:0.2_0": "Dukanipāta",
  "thig2.10:0.3_0": "Sāmātherīgāthā",
  "thig2.10:1.1_0": "“Catukkhattuṁ pañcakkhattuṁ,",
  "thig2.10:1.2_0": "vihārā upanikkhamiṁ;",
  "thig2.10:1.3_0": "Aladdhā cetaso santiṁ,",
  "thig2.10:1.4_0": "citte avasavattinī;",
  "thig2.10:1.5_0": "Tassā me aṭṭhamī ratti,",
  "thig2.10:1.6_0": "yato taṇhā samūhatā.",
  "thig2.10:2.1_0": "Bahūhi dukkhadhammehi,",
  "thig2.10:2.2_0": "appamādaratāya me;",
  "thig2.10:2.3_0": "Taṇhakkhayo anuppatto,",
  "thig2.10:2.4_0": "kataṁ buddhassa sāsanan”ti.",
  "thig2.10:3.1_0": "… Sāmā therī ….",
  "thig2.10:4.1_0": "Dukanipāto niṭṭhito."
}