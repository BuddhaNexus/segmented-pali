{
  "thag1.80:0.1_0": "Theragāthā 1.80",
  "thag1.80:0.2_0": "Ekakanipāta",
  "thag1.80:0.3_0": "Aṭṭhamavagga",
  "thag1.80:0.4_0": "Uggattheragāthā",
  "thag1.80:1.1_0": "“Yaṁ mayā pakataṁ kammaṁ,",
  "thag1.80:1.2_0": "appaṁ vā yadi vā bahuṁ;",
  "thag1.80:1.3_0": "Sabbametaṁ parikkhīṇaṁ,",
  "thag1.80:1.4_0": "natthi dāni punabbhavo”ti.",
  "thag1.80:1.5_0": "… Uggo thero …",
  "thag1.80:1.6_0": "vaggo aṭṭhamo.",
  "thag1.80:2.0_0": "Tassuddānaṁ",
  "thag1.80:2.1_0": "Vacchapālo ca yo thero,",
  "thag1.80:2.2_0": "ātumo māṇavo isi;",
  "thag1.80:2.3_0": "Suyāmano susārado,",
  "thag1.80:2.4_0": "thero yo ca piyañjaho;",
  "thag1.80:2.5_0": "Ārohaputto meṇḍasiro,",
  "thag1.80:2.6_0": "rakkhito uggasavhayoti."
}