{
  "pli-tv-bu-vb-sk45:0.1_0": "Theravāda Vinaya",
  "pli-tv-bu-vb-sk45:0.2_0": "Mahāvibhaṅga",
  "pli-tv-bu-vb-sk45:0.3_0": "Sekhiyakaṇḍa",
  "pli-tv-bu-vb-sk45:0.4_0": "Kabaḷavagga",
  "pli-tv-bu-vb-sk45:0.5_0": "45. Kabaḷāvacchedakasikkhāpada",
  "pli-tv-bu-vb-sk45:1.1_0": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bu-vb-sk45:1.2_0": "Tena kho pana samayena chabbaggiyā bhikkhū kabaḷāvacchedakaṁ bhuñjanti …pe….",
  "pli-tv-bu-vb-sk45:1.3.1_0": "“Na kabaḷāvacchedakaṁ bhuñjissāmīti sikkhā karaṇīyā”ti.",
  "pli-tv-bu-vb-sk45:1.4_0": "Na kabaḷāvacchedakaṁ bhuñjitabbaṁ.",
  "pli-tv-bu-vb-sk45:1.5_0": "Yo anādariyaṁ paṭicca kabaḷāvacchedakaṁ bhuñjati, āpatti dukkaṭassa.",
  "pli-tv-bu-vb-sk45:1.6.1_0": "Anāpatti—",
  "pli-tv-bu-vb-sk45:1.7_0": "asañcicca,",
  "pli-tv-bu-vb-sk45:1.8_0": "assatiyā,",
  "pli-tv-bu-vb-sk45:1.9_0": "ajānantassa,",
  "pli-tv-bu-vb-sk45:1.10_0": "gilānassa,",
  "pli-tv-bu-vb-sk45:1.11_0": "khajjake,",
  "pli-tv-bu-vb-sk45:1.12_0": "phalāphale,",
  "pli-tv-bu-vb-sk45:1.13_0": "uttaribhaṅge,",
  "pli-tv-bu-vb-sk45:1.14_0": "āpadāsu,",
  "pli-tv-bu-vb-sk45:1.15_0": "ummattakassa,",
  "pli-tv-bu-vb-sk45:1.16_0": "ādikammikassāti.",
  "pli-tv-bu-vb-sk45:1.17_0": "Pañcamasikkhāpadaṁ niṭṭhitaṁ."
}