{
  "cp9:0.1_0": "Cariyāpiṭaka",
  "cp9:0.2_0": "Akittivagga",
  "cp9:0.3_0": "Dānapāramī 9",
  "cp9:0.4_0": "Vessantaracariya",
  "cp9:1.1_0": "“Yā me ahosi janikā,",
  "cp9:1.2_0": "phussatī nāma khattiyā;",
  "cp9:1.3_0": "Sā atītāsu jātīsu,",
  "cp9:1.4_0": "sakkassa mahesī piyā.",
  "cp9:2.1_0": "Tassā āyukkhayaṁ ñatvā,",
  "cp9:2.2_0": "devindo etadabravi;",
  "cp9:2.3_0": "‘Dadāmi te dasa vare,",
  "cp9:2.4_0": "varabhadde yadicchasi’.",
  "cp9:3.1_0": "Evaṁ vuttā ca sā devī,",
  "cp9:3.2_0": "sakkaṁ punidamabravi;",
  "cp9:3.3_0": "‘Kiṁ nu me aparādhatthi,",
  "cp9:3.4_0": "kiṁ nu dessā ahaṁ tava;",
  "cp9:3.5_0": "Rammā cāvesi maṁ ṭhānā,",
  "cp9:3.6_0": "vātova dharaṇīruhaṁ’.",
  "cp9:4.1_0": "Evaṁ vutto ca so sakko,",
  "cp9:4.2_0": "puna tassidamabravi;",
  "cp9:4.3_0": "‘Na ceva te kataṁ pāpaṁ,",
  "cp9:4.4_0": "na ca me tvaṁsi appiyā.",
  "cp9:5.1_0": "Ettakaṁyeva te āyu,",
  "cp9:5.2_0": "cavanakālo bhavissati;",
  "cp9:5.3_0": "Paṭiggaṇha mayā dinne,",
  "cp9:5.4_0": "vare dasa varuttame’.",
  "cp9:6.1_0": "Sakkena sā dinnavarā,",
  "cp9:6.2_0": "tuṭṭhahaṭṭhā pamoditā;",
  "cp9:6.3_0": "Mamaṁ abbhantaraṁ katvā,",
  "cp9:6.4_0": "phussatī dasa vare varī.",
  "cp9:7.1_0": "Tato cutā sā phussatī,",
  "cp9:7.2_0": "khattiye upapajjatha;",
  "cp9:7.3_0": "Jetuttaramhi nagare,",
  "cp9:7.4_0": "sañjayena samāgami.",
  "cp9:8.1_0": "Yadāhaṁ phussatiyā kucchiṁ,",
  "cp9:8.2_0": "okkanto piyamātuyā;",
  "cp9:8.3_0": "Mama tejena me mātā,",
  "cp9:8.4_0": "sadā dānaratā ahu.",
  "cp9:9.1_0": "Adhane āture jiṇṇe,",
  "cp9:9.2_0": "yācake addhike jane;",
  "cp9:9.3_0": "Samaṇe brāhmaṇe khīṇe,",
  "cp9:9.4_0": "deti dānaṁ akiñcane.",
  "cp9:10.1_0": "Dasa māse dhārayitvāna,",
  "cp9:10.2_0": "karonte puraṁ padakkhiṇaṁ;",
  "cp9:10.3_0": "Vessānaṁ vīthiyā majjhe,",
  "cp9:10.4_0": "janesi phussatī mamaṁ.",
  "cp9:11.1_0": "Na mayhaṁ mattikaṁ nāmaṁ,",
  "cp9:11.2_0": "napi pettikasambhavaṁ;",
  "cp9:11.3_0": "Jātettha vessavīthiyā,",
  "cp9:11.4_0": "tasmā vessantaro ahu.",
  "cp9:12.1_0": "Yadāhaṁ dārako homi,",
  "cp9:12.2_0": "jātiyā aṭṭhavassiko;",
  "cp9:12.3_0": "Tadā nisajja pāsāde,",
  "cp9:12.4_0": "dānaṁ dātuṁ vicintayiṁ.",
  "cp9:13.1_0": "‘Hadayaṁ dadeyyaṁ cakkhuṁ,",
  "cp9:13.2_0": "maṁsampi rudhirampi ca;",
  "cp9:13.3_0": "Dadeyyaṁ kāyaṁ sāvetvā,",
  "cp9:13.4_0": "yadi koci yācaye mamaṁ’.",
  "cp9:14.1_0": "Sabhāvaṁ cintayantassa,",
  "cp9:14.2_0": "akampitamasaṇṭhitaṁ;",
  "cp9:14.3_0": "Akampi tattha pathavī,",
  "cp9:14.4_0": "sineruvanavaṭaṁsakā.",
  "cp9:15.1_0": "Anvaddhamāse pannarase,",
  "cp9:15.2_0": "puṇṇamāse uposathe;",
  "cp9:15.3_0": "Paccayaṁ nāgamāruyha,",
  "cp9:15.4_0": "dānaṁ dātuṁ upāgamiṁ.",
  "cp9:16.1_0": "Kaliṅgaraṭṭhavisayā,",
  "cp9:16.2_0": "brāhmaṇā upagañchu maṁ;",
  "cp9:16.3_0": "Ayācuṁ maṁ hatthināgaṁ,",
  "cp9:16.4_0": "dhaññaṁ maṅgalasammataṁ.",
  "cp9:17.1_0": "‘Avuṭṭhiko janapado,",
  "cp9:17.2_0": "dubbhikkho chātako mahā;",
  "cp9:17.3_0": "Dadāhi pavaraṁ nāgaṁ,",
  "cp9:17.4_0": "sabbasetaṁ gajuttamaṁ’.",
  "cp9:18.1_0": "Dadāmi na vikampāmi,",
  "cp9:18.2_0": "yaṁ maṁ yācanti brāhmaṇā;",
  "cp9:18.3_0": "Santaṁ nappatigūhāmi,",
  "cp9:18.4_0": "dāne me ramate mano.",
  "cp9:19.1_0": "Na me yācakamanuppatte,",
  "cp9:19.2_0": "paṭikkhepo anucchavo;",
  "cp9:19.3_0": "Mā me bhijji samādānaṁ,",
  "cp9:19.4_0": "dassāmi vipulaṁ gajaṁ.",
  "cp9:20.1_0": "Nāgaṁ gahetvā soṇḍāya,",
  "cp9:20.2_0": "bhiṅgāre ratanāmaye;",
  "cp9:20.3_0": "Jalaṁ hatthe ākiritvā,",
  "cp9:20.4_0": "brāhmaṇānaṁ adaṁ gajaṁ.",
  "cp9:21.1_0": "Punāparaṁ dadantassa,",
  "cp9:21.2_0": "sabbasetaṁ gajuttamaṁ;",
  "cp9:21.3_0": "Tadāpi pathavī kampi,",
  "cp9:21.4_0": "sineruvanavaṭaṁsakā.",
  "cp9:22.1_0": "Tassa nāgassa dānena,",
  "cp9:22.2_0": "sivayo kuddhā samāgatā;",
  "cp9:22.3_0": "Pabbājesuṁ sakā raṭṭhā,",
  "cp9:22.4_0": "‘vaṅkaṁ gacchatu pabbataṁ’.",
  "cp9:23.1_0": "Tesaṁ nicchubhamānānaṁ,",
  "cp9:23.2_0": "akampitamasaṇṭhitaṁ;",
  "cp9:23.3_0": "Mahādānaṁ pavattetuṁ,",
  "cp9:23.4_0": "ekaṁ varamayācisaṁ.",
  "cp9:24.1_0": "Yācitā sivayo sabbe,",
  "cp9:24.2_0": "ekaṁ varamadaṁsu me;",
  "cp9:24.3_0": "Sāvayitvā kaṇṇabheriṁ,",
  "cp9:24.4_0": "mahādānaṁ dadāmahaṁ.",
  "cp9:25.1_0": "Athettha vattatī saddo,",
  "cp9:25.2_0": "tumulo bheravo mahā;",
  "cp9:25.3_0": "Dānenimaṁ nīharanti,",
  "cp9:25.4_0": "puna dānaṁ dadātayaṁ.",
  "cp9:26.1_0": "Hatthiṁ asse rathe datvā,",
  "cp9:26.2_0": "dāsiṁ dāsaṁ gavaṁ dhanaṁ;",
  "cp9:26.3_0": "Mahādānaṁ daditvāna,",
  "cp9:26.4_0": "nagarā nikkhamiṁ tadā.",
  "cp9:27.1_0": "Nikkhamitvāna nagarā,",
  "cp9:27.2_0": "nivattitvā vilokite;",
  "cp9:27.3_0": "Tadāpi pathavī kampi,",
  "cp9:27.4_0": "sineruvanavaṭaṁsakā.",
  "cp9:28.1_0": "Catuvāhiṁ rathaṁ datvā,",
  "cp9:28.2_0": "ṭhatvā cātummahāpathe;",
  "cp9:28.3_0": "Ekākiyo adutiyo,",
  "cp9:28.4_0": "maddideviṁ idamabraviṁ.",
  "cp9:29.1_0": "‘Tvaṁ maddi kaṇhaṁ gaṇhāhi,",
  "cp9:29.2_0": "lahukā esā kaniṭṭhikā;",
  "cp9:29.3_0": "Ahaṁ jāliṁ gahessāmi,",
  "cp9:29.4_0": "garuko bhātiko hi so’.",
  "cp9:30.1_0": "Padumaṁ puṇḍarīkaṁva,",
  "cp9:30.2_0": "maddī kaṇhājinaggahī;",
  "cp9:30.3_0": "Ahaṁ suvaṇṇabimbaṁva,",
  "cp9:30.4_0": "jāliṁ khattiyamaggahiṁ.",
  "cp9:31.1_0": "Abhijātā sukhumālā,",
  "cp9:31.2_0": "khattiyā caturo janā;",
  "cp9:31.3_0": "Visamaṁ samaṁ akkamantā,",
  "cp9:31.4_0": "vaṅkaṁ gacchāma pabbataṁ.",
  "cp9:32.1_0": "Ye keci manujā enti,",
  "cp9:32.2_0": "anumagge paṭippathe;",
  "cp9:32.3_0": "Maggante paṭipucchāma,",
  "cp9:32.4_0": "‘kuhiṁ vaṅkantapabbato’.",
  "cp9:33.1_0": "Te tattha amhe passitvā,",
  "cp9:33.2_0": "karuṇaṁ giramudīrayuṁ;",
  "cp9:33.3_0": "Dukkhante paṭivedenti,",
  "cp9:33.4_0": "dūre vaṅkantapabbato.",
  "cp9:34.1_0": "Yadi passanti pavane,",
  "cp9:34.2_0": "dārakā phaline dume;",
  "cp9:34.3_0": "Tesaṁ phalānaṁ hetumhi,",
  "cp9:34.4_0": "uparodanti dārakā.",
  "cp9:35.1_0": "Rodante dārake disvā,",
  "cp9:35.2_0": "ubbiddhā vipulā dumā;",
  "cp9:35.3_0": "Sayamevoṇamitvāna,",
  "cp9:35.4_0": "upagacchanti dārake.",
  "cp9:36.1_0": "Idaṁ acchariyaṁ disvā,",
  "cp9:36.2_0": "abbhutaṁ lomahaṁsanaṁ;",
  "cp9:36.3_0": "Sāhukāraṁ pavattesi,",
  "cp9:36.4_0": "maddī sabbaṅgasobhanā.",
  "cp9:37.1_0": "Accheraṁ vata lokasmiṁ,",
  "cp9:37.2_0": "abbhutaṁ lomahaṁsanaṁ;",
  "cp9:37.3_0": "Vessantarassa tejena,",
  "cp9:37.4_0": "sayamevoṇatā dumā.",
  "cp9:38.1_0": "Saṅkhipiṁsu pathaṁ yakkhā,",
  "cp9:38.2_0": "anukampāya dārake;",
  "cp9:38.3_0": "Nikkhantadivaseneva,",
  "cp9:38.4_0": "cetaraṭṭhamupāgamuṁ.",
  "cp9:39.1_0": "Saṭṭhirājasahassāni,",
  "cp9:39.2_0": "tadā vasanti mātule;",
  "cp9:39.3_0": "Sabbe pañjalikā hutvā,",
  "cp9:39.4_0": "rodamānā upāgamuṁ.",
  "cp9:40.1_0": "Tattha vattetvā sallāpaṁ,",
  "cp9:40.2_0": "cetehi cetaputtehi;",
  "cp9:40.3_0": "Te tato nikkhamitvāna,",
  "cp9:40.4_0": "vaṅkaṁ agamu pabbataṁ.",
  "cp9:41.1_0": "Āmantayitvā devindo,",
  "cp9:41.2_0": "vissakammaṁ mahiddhikaṁ;",
  "cp9:41.3_0": "Assamaṁ sukataṁ rammaṁ,",
  "cp9:41.4_0": "paṇṇasālaṁ sumāpaya.",
  "cp9:42.1_0": "Sakkassa vacanaṁ sutvā,",
  "cp9:42.2_0": "vissakammo mahiddhiko;",
  "cp9:42.3_0": "Assamaṁ sukataṁ rammaṁ,",
  "cp9:42.4_0": "paṇṇasālaṁ sumāpayi.",
  "cp9:43.1_0": "Ajjhogāhetvā pavanaṁ,",
  "cp9:43.2_0": "appasaddaṁ nirākulaṁ;",
  "cp9:43.3_0": "Caturo janā mayaṁ tattha,",
  "cp9:43.4_0": "vasāma pabbatantare.",
  "cp9:44.1_0": "Ahañca maddidevī ca,",
  "cp9:44.2_0": "jālī kaṇhājinā cubho;",
  "cp9:44.3_0": "Aññamaññaṁ sokanudā,",
  "cp9:44.4_0": "vasāma assame tadā.",
  "cp9:45.1_0": "Dārake anurakkhanto,",
  "cp9:45.2_0": "asuñño homi assame;",
  "cp9:45.3_0": "Maddī phalaṁ āharitvā,",
  "cp9:45.4_0": "poseti sā tayo jane.",
  "cp9:46.1_0": "Pavane vasamānassa,",
  "cp9:46.2_0": "addhiko maṁ upāgami;",
  "cp9:46.3_0": "Āyāci puttake mayhaṁ,",
  "cp9:46.4_0": "jāliṁ kaṇhājinaṁ cubho.",
  "cp9:47.1_0": "Yācakaṁ upagataṁ disvā,",
  "cp9:47.2_0": "hāso me upapajjatha;",
  "cp9:47.3_0": "Ubho putte gahetvāna,",
  "cp9:47.4_0": "adāsiṁ brāhmaṇe tadā.",
  "cp9:48.1_0": "Sake putte cajantassa,",
  "cp9:48.2_0": "jūjake brāhmaṇe yadā;",
  "cp9:48.3_0": "Tadāpi pathavī kampi,",
  "cp9:48.4_0": "sineruvanavaṭaṁsakā.",
  "cp9:49.1_0": "Punadeva sakko oruyha,",
  "cp9:49.2_0": "hutvā brāhmaṇasannibho;",
  "cp9:49.3_0": "Āyāci maṁ maddideviṁ,",
  "cp9:49.4_0": "sīlavantiṁ patibbataṁ.",
  "cp9:50.1_0": "Maddiṁ hatthe gahetvāna,",
  "cp9:50.2_0": "udakañjali pūriya;",
  "cp9:50.3_0": "Pasannamanasaṅkappo,",
  "cp9:50.4_0": "tassa maddiṁ adāsahaṁ.",
  "cp9:51.1_0": "Maddiyā dīyamānāya,",
  "cp9:51.2_0": "gagane devā pamoditā;",
  "cp9:51.3_0": "Tadāpi pathavī kampi,",
  "cp9:51.4_0": "sineruvanavaṭaṁsakā.",
  "cp9:52.1_0": "Jāliṁ kaṇhājinaṁ dhītaṁ,",
  "cp9:52.2_0": "maddideviṁ patibbataṁ;",
  "cp9:52.3_0": "Cajamāno na cintesiṁ,",
  "cp9:52.4_0": "bodhiyāyeva kāraṇā.",
  "cp9:53.1_0": "Na me dessā ubho puttā,",
  "cp9:53.2_0": "maddidevī na dessiyā;",
  "cp9:53.3_0": "Sabbaññutaṁ piyaṁ mayhaṁ,",
  "cp9:53.4_0": "tasmā piye adāsahaṁ.",
  "cp9:54.1_0": "Punāparaṁ brahāraññe,",
  "cp9:54.2_0": "mātāpitusamāgame;",
  "cp9:54.3_0": "Karuṇaṁ paridevante,",
  "cp9:54.4_0": "sallapante sukhaṁ dukhaṁ.",
  "cp9:55.1_0": "Hirottappena garunā,",
  "cp9:55.2_0": "ubhinnaṁ upasaṅkami;",
  "cp9:55.3_0": "Tadāpi pathavī kampi,",
  "cp9:55.4_0": "sineruvanavaṭaṁsakā.",
  "cp9:56.1_0": "Punāparaṁ brahāraññā,",
  "cp9:56.2_0": "nikkhamitvā sañātibhi;",
  "cp9:56.3_0": "Pavisāmi puraṁ rammaṁ,",
  "cp9:56.4_0": "jetuttaraṁ puruttamaṁ.",
  "cp9:57.1_0": "Ratanāni satta vassiṁsu,",
  "cp9:57.2_0": "mahāmegho pavassatha;",
  "cp9:57.3_0": "Tadāpi pathavī kampi,",
  "cp9:57.4_0": "sineruvanavaṭaṁsakā.",
  "cp9:58.1_0": "Acetanāyaṁ pathavī,",
  "cp9:58.2_0": "aviññāya sukhaṁ dukhaṁ;",
  "cp9:58.3_0": "Sāpi dānabalā mayhaṁ,",
  "cp9:58.4_0": "sattakkhattuṁ pakampathā”ti.",
  "cp9:59.1_0": "Vessantaracariyaṁ navamaṁ."
}