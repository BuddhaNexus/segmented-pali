{
  "ja355:0.1_0": "Jātaka",
  "ja355:0.2_0": "Pañcakanipāta",
  "ja355:0.3_0": "Maṇikuṇḍalavagga",
  "ja355:0.4_0": "5. Ghaṭajātaka",
  "ja355:1.1_0": "“Aññe socanti rodanti,",
  "ja355:1.2_0": "aññe assumukhā janā;",
  "ja355:1.3_0": "Pasannamukhavaṇṇosi,",
  "ja355:1.4_0": "kasmā ghaṭa na socasi”.",
  "ja355:2.1_0": "“Nābbhatītaharo soko,",
  "ja355:2.2_0": "nānāgatasukhāvaho;",
  "ja355:2.3_0": "Tasmā dhaṅka na socāmi,",
  "ja355:2.4_0": "natthi soke dutīyatā.",
  "ja355:3.1_0": "Socaṁ paṇḍu kiso hoti,",
  "ja355:3.2_0": "bhattañcassa na ruccati;",
  "ja355:3.3_0": "Amittā sumanā honti,",
  "ja355:3.4_0": "sallaviddhassa ruppato.",
  "ja355:4.1_0": "Gāme vā yadi vāraññe,",
  "ja355:4.2_0": "ninne vā yadi vā thale;",
  "ja355:4.3_0": "Ṭhitaṁ maṁ nāgamissati,",
  "ja355:4.4_0": "evaṁ diṭṭhapado ahaṁ.",
  "ja355:5.1_0": "Yassattā nālamekova,",
  "ja355:5.2_0": "sabbakāmarasāharo;",
  "ja355:5.3_0": "Sabbāpi pathavī tassa,",
  "ja355:5.4_0": "na sukhaṁ āvahissatī”ti.",
  "ja355:6.1_0": "Ghaṭajātakaṁ pañcamaṁ."
}