{
  "thig1.14:0.1_0": "Therīgāthā 1.14",
  "thig1.14:0.2_0": "Ekakanipāta",
  "thig1.14:0.3_0": "Sumanātherīgāthā",
  "thig1.14:1.1_0": "“Dhātuyo dukkhato disvā,",
  "thig1.14:1.2_0": "mā jātiṁ punarāgami;",
  "thig1.14:1.3_0": "Bhave chandaṁ virājetvā,",
  "thig1.14:1.4_0": "upasantā carissasī”ti.",
  "thig1.14:2.1_0": "… Sumanā therī …."
}