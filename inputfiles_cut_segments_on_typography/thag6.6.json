{
  "thag6.6:0.1_0": "Theragāthā 6.6",
  "thag6.6:0.2_0": "Chakkanipāta",
  "thag6.6:0.3_0": "Paṭhamavagga",
  "thag6.6:0.4_0": "Sappadāsattheragāthā",
  "thag6.6:1.1_0": "“Paṇṇavīsativassāni,",
  "thag6.6:1.2_0": "yato pabbajito ahaṁ;",
  "thag6.6:1.3_0": "Accharāsaṅghātamattampi,",
  "thag6.6:1.4_0": "cetosantimanajjhagaṁ.",
  "thag6.6:2.1_0": "Aladdhā cittassekaggaṁ,",
  "thag6.6:2.2_0": "kāmarāgena aṭṭito;",
  "thag6.6:2.3_0": "Bāhā paggayha kandanto,",
  "thag6.6:2.4_0": "vihārā upanikkhamiṁ.",
  "thag6.6:3.1_0": "Satthaṁ vā āharissāmi,",
  "thag6.6:3.2_0": "Ko attho jīvitena me;",
  "thag6.6:3.3_0": "Kathaṁ hi sikkhaṁ paccakkhaṁ,",
  "thag6.6:3.4_0": "Kālaṁ kubbetha mādiso.",
  "thag6.6:4.1_0": "Tadāhaṁ khuramādāya,",
  "thag6.6:4.2_0": "mañcakamhi upāvisiṁ;",
  "thag6.6:4.3_0": "Parinīto khuro āsi,",
  "thag6.6:4.4_0": "dhamaniṁ chettumattano.",
  "thag6.6:5.1_0": "Tato me manasīkāro,",
  "thag6.6:5.2_0": "yoniso udapajjatha;",
  "thag6.6:5.3_0": "Ādīnavo pāturahu,",
  "thag6.6:5.4_0": "nibbidā samatiṭṭhatha.",
  "thag6.6:6.1_0": "Tato cittaṁ vimucci me,",
  "thag6.6:6.2_0": "passa dhammasudhammataṁ;",
  "thag6.6:6.3_0": "Tisso vijjā anuppattā,",
  "thag6.6:6.4_0": "kataṁ buddhassa sāsanan”ti.",
  "thag6.6:6.5_0": "… Sappadāso thero …."
}