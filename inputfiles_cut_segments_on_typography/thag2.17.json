{
  "thag2.17:0.1_0": "Theragāthā 2.17",
  "thag2.17:0.2_0": "Dukanipāta",
  "thag2.17:0.3_0": "Dutiyavagga",
  "thag2.17:0.4_0": "Tissattheragāthā",
  "thag2.17:1.1_0": "“Bahū sapatte labhati,",
  "thag2.17:1.2_0": "muṇḍo saṅghāṭipāruto;",
  "thag2.17:1.3_0": "Lābhī annassa pānassa,",
  "thag2.17:1.4_0": "vatthassa sayanassa ca.",
  "thag2.17:2.1_0": "Etamādīnavaṁ ñatvā,",
  "thag2.17:2.2_0": "sakkāresu mahabbhayaṁ;",
  "thag2.17:2.3_0": "Appalābho anavassuto,",
  "thag2.17:2.4_0": "sato bhikkhu paribbaje”ti.",
  "thag2.17:2.5_0": "… Tisso thero …."
}