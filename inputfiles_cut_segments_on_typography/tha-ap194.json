{
  "tha-ap194:0.1_0": "Therāpadāna",
  "tha-ap194:0.2_0": "Tamālapupphiyavagga",
  "tha-ap194:0.3_0": "2 Tiṇasanthārakattheraapadāna",
  "tha-ap194:1.1_0": "“Yaṁ dāyavāsiko isi,",
  "tha-ap194:1.2_0": "tiṇaṁ lāyati satthuno;",
  "tha-ap194:1.3_0": "Sabbe padakkhiṇāvaṭṭā,",
  "tha-ap194:1.4_0": "pathabyā nipatiṁsu te.",
  "tha-ap194:2.1_0": "Tamahaṁ tiṇamādāya,",
  "tha-ap194:2.2_0": "santhariṁ dharaṇuttame;",
  "tha-ap194:2.3_0": "Tīṇeva tālapattāni,",
  "tha-ap194:2.4_0": "āharitvānahaṁ tadā.",
  "tha-ap194:3.1_0": "Tiṇena chadanaṁ katvā,",
  "tha-ap194:3.2_0": "siddhatthassa adāsahaṁ;",
  "tha-ap194:3.3_0": "Sattāhaṁ dhārayuṁ tassa,",
  "tha-ap194:3.4_0": "devamānusasatthuno.",
  "tha-ap194:4.1_0": "Catunnavutito kappe,",
  "tha-ap194:4.2_0": "yaṁ tiṇaṁ adadiṁ tadā;",
  "tha-ap194:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap194:4.4_0": "tiṇadānassidaṁ phalaṁ.",
  "tha-ap194:5.1_0": "Pañcasaṭṭhimhito kappe,",
  "tha-ap194:5.2_0": "cattārosuṁ mahaddhanā;",
  "tha-ap194:5.3_0": "Sattaratanasampannā,",
  "tha-ap194:5.4_0": "cakkavattī mahabbalā.",
  "tha-ap194:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap194:6.2_0": "…pe…",
  "tha-ap194:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap194:7.1_0": "Itthaṁ sudaṁ āyasmā tiṇasanthārako thero imā gāthāyo abhāsitthāti.",
  "tha-ap194:8.1_0": "Tiṇasanthārakattherassāpadānaṁ dutiyaṁ."
}