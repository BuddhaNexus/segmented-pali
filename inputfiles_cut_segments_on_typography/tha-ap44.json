{
  "tha-ap44:0.1_0": "Therāpadāna",
  "tha-ap44:0.2_0": "Upālivagga",
  "tha-ap44:0.3_0": "2 Soṇakoḷivisattheraapadāna",
  "tha-ap44:1.1_0": "“Anomadassissa munino,",
  "tha-ap44:1.2_0": "lokajeṭṭhassa tādino;",
  "tha-ap44:1.3_0": "Sudhāya lepanaṁ katvā,",
  "tha-ap44:1.4_0": "caṅkamaṁ kārayiṁ ahaṁ.",
  "tha-ap44:2.1_0": "Nānāvaṇṇehi pupphehi,",
  "tha-ap44:2.2_0": "caṅkamaṁ santhariṁ ahaṁ;",
  "tha-ap44:2.3_0": "Ākāse vitānaṁ katvā,",
  "tha-ap44:2.4_0": "bhojayiṁ buddhamuttamaṁ.",
  "tha-ap44:3.1_0": "Añjaliṁ paggahetvāna,",
  "tha-ap44:3.2_0": "abhivādetvāna subbataṁ;",
  "tha-ap44:3.3_0": "Dīghasālaṁ bhagavato,",
  "tha-ap44:3.4_0": "niyyādesimahaṁ tadā.",
  "tha-ap44:4.1_0": "Mama saṅkappamaññāya,",
  "tha-ap44:4.2_0": "satthā loke anuttaro;",
  "tha-ap44:4.3_0": "Paṭiggahesi bhagavā,",
  "tha-ap44:4.4_0": "anukampāya cakkhumā.",
  "tha-ap44:5.1_0": "Paṭiggahetvāna sambuddho,",
  "tha-ap44:5.2_0": "dakkhiṇeyyo sadevake;",
  "tha-ap44:5.3_0": "Bhikkhusaṅghe nisīditvā,",
  "tha-ap44:5.4_0": "imā gāthā abhāsatha.",
  "tha-ap44:6.1_0": "‘Yo so haṭṭhena cittena,",
  "tha-ap44:6.2_0": "dīghasālaṁ adāsi me;",
  "tha-ap44:6.3_0": "Tamahaṁ kittayissāmi,",
  "tha-ap44:6.4_0": "suṇātha mama bhāsato.",
  "tha-ap44:7.1_0": "Imassa maccukālamhi,",
  "tha-ap44:7.2_0": "puññakammasamaṅgino;",
  "tha-ap44:7.3_0": "Sahassayuttassaratho,",
  "tha-ap44:7.4_0": "upaṭṭhissati tāvade.",
  "tha-ap44:8.1_0": "Tena yānenayaṁ poso,",
  "tha-ap44:8.2_0": "devalokaṁ gamissati;",
  "tha-ap44:8.3_0": "Anumodissare devā,",
  "tha-ap44:8.4_0": "sampatte kusalabbhave.",
  "tha-ap44:9.1_0": "Mahārahaṁ byamhaṁ seṭṭhaṁ,",
  "tha-ap44:9.2_0": "ratanamattikalepanaṁ;",
  "tha-ap44:9.3_0": "Kūṭāgāravarūpetaṁ,",
  "tha-ap44:9.4_0": "byamhaṁ ajjhāvasissati.",
  "tha-ap44:10.1_0": "Tiṁsakappasahassāni,",
  "tha-ap44:10.2_0": "devaloke ramissati;",
  "tha-ap44:10.3_0": "Pañcavīsati kappāni,",
  "tha-ap44:10.4_0": "devarājā bhavissati.",
  "tha-ap44:11.1_0": "Sattasattatikkhattuñca,",
  "tha-ap44:11.2_0": "cakkavattī bhavissati;",
  "tha-ap44:11.3_0": "Yasodharasanāmā te,",
  "tha-ap44:11.4_0": "sabbepi ekanāmakā.",
  "tha-ap44:12.1_0": "Dve sampattī anubhotvā,",
  "tha-ap44:12.2_0": "Vaḍḍhetvā puññasañcayaṁ;",
  "tha-ap44:12.3_0": "Aṭṭhavīsatikappamhi,",
  "tha-ap44:12.4_0": "Cakkavattī bhavissati.",
  "tha-ap44:13.1_0": "Tatrāpi byamhaṁ pavaraṁ,",
  "tha-ap44:13.2_0": "vissakammena māpitaṁ;",
  "tha-ap44:13.3_0": "Dasasaddāvivittaṁ taṁ,",
  "tha-ap44:13.4_0": "puramajjhāvasissati.",
  "tha-ap44:14.1_0": "Aparimeyye ito kappe,",
  "tha-ap44:14.2_0": "bhūmipālo mahiddhiko;",
  "tha-ap44:14.3_0": "Okkāko nāma nāmena,",
  "tha-ap44:14.4_0": "rājā raṭṭhe bhavissati.",
  "tha-ap44:15.1_0": "Soḷasitthisahassānaṁ,",
  "tha-ap44:15.2_0": "sabbāsaṁ pavarā ca sā;",
  "tha-ap44:15.3_0": "Abhijātā khattiyānī,",
  "tha-ap44:15.4_0": "nava putte janessati.",
  "tha-ap44:16.1_0": "Nava putte janetvāna,",
  "tha-ap44:16.2_0": "khattiyānī marissati;",
  "tha-ap44:16.3_0": "Taruṇī ca piyā kaññā,",
  "tha-ap44:16.4_0": "mahesittaṁ karissati.",
  "tha-ap44:17.1_0": "Okkākaṁ tosayitvāna,",
  "tha-ap44:17.2_0": "varaṁ kaññā labhissati;",
  "tha-ap44:17.3_0": "Varaṁ laddhāna sā kaññā,",
  "tha-ap44:17.4_0": "putte pabbājayissati.",
  "tha-ap44:18.1_0": "Pabbājitā ca te sabbe,",
  "tha-ap44:18.2_0": "gamissanti naguttamaṁ;",
  "tha-ap44:18.3_0": "Jātibhedabhayā sabbe,",
  "tha-ap44:18.4_0": "bhaginīhi vasissare.",
  "tha-ap44:19.1_0": "Ekā ca kaññā byādhīhi,",
  "tha-ap44:19.2_0": "bhavissati parikkhatā;",
  "tha-ap44:19.3_0": "Mā no jāti pabhijjīti,",
  "tha-ap44:19.4_0": "nikhaṇissanti khattiyā.",
  "tha-ap44:20.1_0": "Khattiyo nīharitvāna,",
  "tha-ap44:20.2_0": "tāya saddhiṁ vasissati;",
  "tha-ap44:20.3_0": "Bhavissati tadā bhedo,",
  "tha-ap44:20.4_0": "okkākakulasambhavo.",
  "tha-ap44:21.1_0": "Tesaṁ pajā bhavissanti,",
  "tha-ap44:21.2_0": "koḷiyā nāma jātiyā;",
  "tha-ap44:21.3_0": "Tattha mānusakaṁ bhogaṁ,",
  "tha-ap44:21.4_0": "anubhossatinappakaṁ.",
  "tha-ap44:22.1_0": "Tamhā kāyā cavitvāna,",
  "tha-ap44:22.2_0": "devalokaṁ gamissati;",
  "tha-ap44:22.3_0": "Tatrāpi pavaraṁ byamhaṁ,",
  "tha-ap44:22.4_0": "labhissati manoramaṁ.",
  "tha-ap44:23.1_0": "Devalokā cavitvāna,",
  "tha-ap44:23.2_0": "sukkamūlena codito;",
  "tha-ap44:23.3_0": "Āgantvāna manussattaṁ,",
  "tha-ap44:23.4_0": "soṇo nāma bhavissati.",
  "tha-ap44:24.1_0": "Āraddhavīriyo pahitatto,",
  "tha-ap44:24.2_0": "Padahaṁ satthu sāsane;",
  "tha-ap44:24.3_0": "Sabbāsave pariññāya,",
  "tha-ap44:24.4_0": "Nibbāyissatināsavo.",
  "tha-ap44:25.1_0": "Anantadassī bhagavā,",
  "tha-ap44:25.2_0": "gotamo sakyapuṅgavo;",
  "tha-ap44:25.3_0": "Visesaññū mahāvīro,",
  "tha-ap44:25.4_0": "aggaṭṭhāne ṭhapessati’.",
  "tha-ap44:26.1_0": "Vuṭṭhamhi deve caturaṅgulamhi,",
  "tha-ap44:26.2_0": "Tiṇe anileritaaṅgaṇamhi;",
  "tha-ap44:26.3_0": "Ṭhatvāna yogassa payuttatādino,",
  "tha-ap44:26.4_0": "Tatottariṁ pāramatā na vijjati.",
  "tha-ap44:27.1_0": "Uttame damathe danto,",
  "tha-ap44:27.2_0": "cittaṁ me supaṇīhitaṁ;",
  "tha-ap44:27.3_0": "Bhāro me ohito sabbo,",
  "tha-ap44:27.4_0": "nibbutomhi anāsavo.",
  "tha-ap44:28.1_0": "Aṅgīraso mahānāgo,",
  "tha-ap44:28.2_0": "abhijātova kesarī;",
  "tha-ap44:28.3_0": "Bhikkhusaṅghe nisīditvā,",
  "tha-ap44:28.4_0": "etadagge ṭhapesi maṁ.",
  "tha-ap44:29.1_0": "Paṭisambhidā catasso,",
  "tha-ap44:29.2_0": "…pe…",
  "tha-ap44:29.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap44:30.1_0": "Itthaṁ sudaṁ āyasmā soṇo koḷiviso thero imā gāthāyo abhāsitthāti.",
  "tha-ap44:31.1_0": "Soṇakoḷivisattherassāpadānaṁ dutiyaṁ."
}