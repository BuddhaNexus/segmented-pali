{
  "snp5.17:0.1_0": "Sutta Nipāta 5.17",
  "snp5.17:0.2_0": "Piṅgiyamāṇavapucchā",
  "snp5.17:1.1_0": "“Jiṇṇohamasmi abalo vītavaṇṇo,",
  "snp5.17:1.2_0": "(iccāyasmā piṅgiyo)",
  "snp5.17:1.3_0": "Nettā na suddhā savanaṁ na phāsu;",
  "snp5.17:1.4_0": "Māhaṁ nassaṁ momuho antarāva,",
  "snp5.17:1.5_0": "Ācikkha dhammaṁ yamahaṁ vijaññaṁ;",
  "snp5.17:1.6_0": "Jātijarāya idha vippahānaṁ”.",
  "snp5.17:2.1_0": "“Disvāna rūpesu vihaññamāne,",
  "snp5.17:2.2_0": "(piṅgiyāti bhagavā)",
  "snp5.17:2.3_0": "Ruppanti rūpesu janā pamattā;",
  "snp5.17:2.4_0": "Tasmā tuvaṁ piṅgiya appamatto,",
  "snp5.17:2.5_0": "Jahassu rūpaṁ apunabbhavāya”.",
  "snp5.17:3.1_0": "“Disā catasso vidisā catasso,",
  "snp5.17:3.2_0": "Uddhaṁ adho dasa disā imāyo;",
  "snp5.17:3.3_0": "Na tuyhaṁ adiṭṭhaṁ asutaṁ amutaṁ,",
  "snp5.17:3.4_0": "Atho aviññātaṁ kiñcanamatthi loke;",
  "snp5.17:3.5_0": "Ācikkha dhammaṁ yamahaṁ vijaññaṁ,",
  "snp5.17:3.6_0": "Jātijarāya idha vippahānaṁ”.",
  "snp5.17:4.1_0": "“Taṇhādhipanne manuje pekkhamāno,",
  "snp5.17:4.2_0": "(piṅgiyāti bhagavā)",
  "snp5.17:4.3_0": "Santāpajāte jarasā parete;",
  "snp5.17:4.4_0": "Tasmā tuvaṁ piṅgiya appamatto,",
  "snp5.17:4.5_0": "Jahassu taṇhaṁ apunabbhavāyā”ti.",
  "snp5.17:4.6_0": "Piṅgiyamāṇavapucchā soḷasamā."
}