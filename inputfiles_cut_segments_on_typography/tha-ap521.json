{
  "tha-ap521:0.1_0": "Therāpadāna",
  "tha-ap521:0.2_0": "Phaladāyakavagga",
  "tha-ap521:0.3_0": "9. Sabbaphaladāyakattheraapadāna",
  "tha-ap521:1.1_0": "“Varuṇo nāma nāmena,",
  "tha-ap521:1.2_0": "brāhmaṇo mantapāragū;",
  "tha-ap521:1.3_0": "Chaḍḍetvā dasa puttāni,",
  "tha-ap521:1.4_0": "vanamajjhogahiṁ tadā.",
  "tha-ap521:2.1_0": "Assamaṁ sukataṁ katvā,",
  "tha-ap521:2.2_0": "suvibhattaṁ manoramaṁ;",
  "tha-ap521:2.3_0": "Paṇṇasālaṁ karitvāna,",
  "tha-ap521:2.4_0": "vasāmi vipine ahaṁ.",
  "tha-ap521:3.1_0": "Padumuttaro lokavidū,",
  "tha-ap521:3.2_0": "āhutīnaṁ paṭiggaho;",
  "tha-ap521:3.3_0": "Mamuddharitukāmo so,",
  "tha-ap521:3.4_0": "āgacchi mama assamaṁ.",
  "tha-ap521:4.1_0": "Yāvatā vanasaṇḍamhi,",
  "tha-ap521:4.2_0": "obhāso vipulo ahu;",
  "tha-ap521:4.3_0": "Buddhassa ānubhāvena,",
  "tha-ap521:4.4_0": "pajjalī vipinaṁ tadā.",
  "tha-ap521:5.1_0": "Disvāna taṁ pāṭihīraṁ,",
  "tha-ap521:5.2_0": "buddhaseṭṭhassa tādino;",
  "tha-ap521:5.3_0": "Pattapuṭaṁ gahetvāna,",
  "tha-ap521:5.4_0": "phalena pūjayiṁ ahaṁ.",
  "tha-ap521:6.1_0": "Upagantvāna sambuddhaṁ,",
  "tha-ap521:6.2_0": "sahakhārimadāsahaṁ;",
  "tha-ap521:6.3_0": "Anukampāya me buddho,",
  "tha-ap521:6.4_0": "idaṁ vacanamabravi.",
  "tha-ap521:7.1_0": "‘Khāribhāraṁ gahetvāna,",
  "tha-ap521:7.2_0": "pacchato ehi me tuvaṁ;",
  "tha-ap521:7.3_0": "Paribhutte ca saṅghamhi,",
  "tha-ap521:7.4_0": "puññaṁ tava bhavissati’.",
  "tha-ap521:8.1_0": "Puṭakantaṁ gahetvāna,",
  "tha-ap521:8.2_0": "bhikkhusaṅghassadāsahaṁ;",
  "tha-ap521:8.3_0": "Tattha cittaṁ pasādetvā,",
  "tha-ap521:8.4_0": "tusitaṁ upapajjahaṁ.",
  "tha-ap521:9.1_0": "Tattha dibbehi naccehi,",
  "tha-ap521:9.2_0": "gītehi vāditehi ca;",
  "tha-ap521:9.3_0": "Puññakammena saṁyuttaṁ,",
  "tha-ap521:9.4_0": "anubhomi sadā sukhaṁ.",
  "tha-ap521:10.1_0": "Yaṁ yaṁ yonupapajjāmi,",
  "tha-ap521:10.2_0": "devattaṁ atha mānusaṁ;",
  "tha-ap521:10.3_0": "Bhoge me ūnatā natthi,",
  "tha-ap521:10.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap521:11.1_0": "Yāvatā caturo dīpā,",
  "tha-ap521:11.2_0": "sasamuddā sapabbatā;",
  "tha-ap521:11.3_0": "Phalaṁ buddhassa datvāna,",
  "tha-ap521:11.4_0": "issaraṁ kārayāmahaṁ.",
  "tha-ap521:12.1_0": "Yāvatā me pakkhigaṇā,",
  "tha-ap521:12.2_0": "ākāse uppatanti ce;",
  "tha-ap521:12.3_0": "Tepi maṁ vasamanventi,",
  "tha-ap521:12.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap521:13.1_0": "Yāvatā vanasaṇḍamhi,",
  "tha-ap521:13.2_0": "yakkhā bhūtā ca rakkhasā;",
  "tha-ap521:13.3_0": "Kumbhaṇḍā garuḷā cāpi,",
  "tha-ap521:13.4_0": "pāricariyaṁ upenti me.",
  "tha-ap521:14.1_0": "Kummā soṇā madhukārā,",
  "tha-ap521:14.2_0": "ḍaṁsā ca makasā ubho;",
  "tha-ap521:14.3_0": "Tepi maṁ vasamanventi,",
  "tha-ap521:14.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap521:15.1_0": "Supaṇṇā nāma sakuṇā,",
  "tha-ap521:15.2_0": "pakkhijātā mahabbalā;",
  "tha-ap521:15.3_0": "Tepi maṁ saraṇaṁ yanti,",
  "tha-ap521:15.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap521:16.1_0": "Yepi dīghāyukā nāgā,",
  "tha-ap521:16.2_0": "iddhimanto mahāyasā;",
  "tha-ap521:16.3_0": "Tepi maṁ vasamanventi,",
  "tha-ap521:16.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap521:17.1_0": "Sīhā byagghā ca dīpī ca,",
  "tha-ap521:17.2_0": "acchakokataracchakā;",
  "tha-ap521:17.3_0": "Tepi maṁ vasamanventi,",
  "tha-ap521:17.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap521:18.1_0": "Osadhītiṇavāsī ca,",
  "tha-ap521:18.2_0": "ye ca ākāsavāsino;",
  "tha-ap521:18.3_0": "Sabbe maṁ saraṇaṁ yanti,",
  "tha-ap521:18.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap521:19.1_0": "Sududdasaṁ sunipuṇaṁ,",
  "tha-ap521:19.2_0": "gambhīraṁ suppakāsitaṁ;",
  "tha-ap521:19.3_0": "Phassayitvā viharāmi,",
  "tha-ap521:19.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap521:20.1_0": "Vimokkhe aṭṭha phusitvā,",
  "tha-ap521:20.2_0": "viharāmi anāsavo;",
  "tha-ap521:20.3_0": "Ātāpī nipako cāhaṁ,",
  "tha-ap521:20.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap521:21.1_0": "Ye phalaṭṭhā buddhaputtā,",
  "tha-ap521:21.2_0": "khīṇadosā mahāyasā;",
  "tha-ap521:21.3_0": "Ahamaññataro tesaṁ,",
  "tha-ap521:21.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap521:22.1_0": "Abhiññāpāramiṁ gantvā,",
  "tha-ap521:22.2_0": "sukkamūlena codito;",
  "tha-ap521:22.3_0": "Sabbāsave pariññāya,",
  "tha-ap521:22.4_0": "viharāmi anāsavo.",
  "tha-ap521:23.1_0": "Tevijjā iddhipattā ca,",
  "tha-ap521:23.2_0": "buddhaputtā mahāyasā;",
  "tha-ap521:23.3_0": "Dibbasotasamāpannā,",
  "tha-ap521:23.4_0": "tesaṁ aññataro ahaṁ.",
  "tha-ap521:24.1_0": "Satasahassito kappe,",
  "tha-ap521:24.2_0": "yaṁ phalaṁ adadiṁ tadā;",
  "tha-ap521:24.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap521:24.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap521:25.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap521:25.2_0": "…pe…",
  "tha-ap521:25.3_0": "viharāmi anāsavo.",
  "tha-ap521:26.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap521:26.2_0": "…pe…",
  "tha-ap521:26.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap521:27.1_0": "Paṭisambhidā catasso,",
  "tha-ap521:27.2_0": "…pe…",
  "tha-ap521:27.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap521:28.1_0": "Itthaṁ sudaṁ āyasmā sabbaphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap521:29.1_0": "Sabbaphaladāyakattherassāpadānaṁ navamaṁ."
}