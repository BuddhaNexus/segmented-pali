{
  "tha-ap57:0.1_0": "Therāpadāna",
  "tha-ap57:0.2_0": "Bījanivagga",
  "tha-ap57:0.3_0": "5 Opavayhattheraapadāna",
  "tha-ap57:1.1_0": "“Padumuttarabuddhassa,",
  "tha-ap57:1.2_0": "ājānīyamadāsahaṁ;",
  "tha-ap57:1.3_0": "Niyyādetvāna sambuddhe,",
  "tha-ap57:1.4_0": "agamāsiṁ sakaṁ gharaṁ.",
  "tha-ap57:2.1_0": "Devalo nāma nāmena,",
  "tha-ap57:2.2_0": "satthuno aggasāvako;",
  "tha-ap57:2.3_0": "Varadhammassa dāyādo,",
  "tha-ap57:2.4_0": "āgacchi mama santikaṁ.",
  "tha-ap57:3.1_0": "Sapattabhāro bhagavā,",
  "tha-ap57:3.2_0": "ājāneyyo na kappati;",
  "tha-ap57:3.3_0": "Tava saṅkappamaññāya,",
  "tha-ap57:3.4_0": "adhivāsesi cakkhumā.",
  "tha-ap57:4.1_0": "Agghāpetvā vātajavaṁ,",
  "tha-ap57:4.2_0": "sindhavaṁ sīghavāhanaṁ;",
  "tha-ap57:4.3_0": "Padumuttarabuddhassa,",
  "tha-ap57:4.4_0": "khamanīyamadāsahaṁ.",
  "tha-ap57:5.1_0": "Yaṁ yaṁ yonupapajjāmi,",
  "tha-ap57:5.2_0": "devattaṁ atha mānusaṁ;",
  "tha-ap57:5.3_0": "Khamanīyaṁ vātajavaṁ,",
  "tha-ap57:5.4_0": "cittaṁ nibbattate mama.",
  "tha-ap57:6.1_0": "Lābhaṁ tesaṁ suladdhaṁva,",
  "tha-ap57:6.2_0": "ye labhantupasampadaṁ;",
  "tha-ap57:6.3_0": "Punapi payirupāseyyaṁ,",
  "tha-ap57:6.4_0": "buddho loke sace bhave.",
  "tha-ap57:7.1_0": "Aṭṭhavīsatikkhattuṁhaṁ,",
  "tha-ap57:7.2_0": "rājā āsiṁ mahabbalo;",
  "tha-ap57:7.3_0": "Cāturanto vijitāvī,",
  "tha-ap57:7.4_0": "jambusaṇḍassa issaro.",
  "tha-ap57:8.1_0": "Idaṁ pacchimakaṁ mayhaṁ,",
  "tha-ap57:8.2_0": "carimo vattate bhavo;",
  "tha-ap57:8.3_0": "Pattosmi acalaṁ ṭhānaṁ,",
  "tha-ap57:8.4_0": "hitvā jayaparājayaṁ.",
  "tha-ap57:9.1_0": "Catutiṁsasahassamhi,",
  "tha-ap57:9.2_0": "mahātejosi khattiyo;",
  "tha-ap57:9.3_0": "Sattaratanasampanno,",
  "tha-ap57:9.4_0": "cakkavattī mahabbalo.",
  "tha-ap57:10.1_0": "Paṭisambhidā catasso,",
  "tha-ap57:10.2_0": "…pe…",
  "tha-ap57:10.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap57:11.1_0": "Itthaṁ sudaṁ āyasmā opavayho thero imā gāthāyo abhāsitthāti.",
  "tha-ap57:12.1_0": "Opavayhattherassāpadānaṁ pañcamaṁ."
}