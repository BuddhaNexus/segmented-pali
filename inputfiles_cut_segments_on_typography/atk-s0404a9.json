{
  "atk-s0404a9:0_0": "(10) 5. Sāmaññavaggo",
  "atk-s0404a9:1_0": "91.",
  "atk-s0404a9:1_1": "Ito paraṁ atha kho bojjhā upāsikātiādīsu bojjhā upāsikā,",
  "atk-s0404a9:1_2": "sirimā upāsikā,",
  "atk-s0404a9:1_3": "padumā upāsikā,",
  "atk-s0404a9:1_4": "sutanā upāsikā,",
  "atk-s0404a9:1_5": "manujā upāsikā,",
  "atk-s0404a9:1_6": "uttarā upāsikā,",
  "atk-s0404a9:1_7": "muttā upāsikā,",
  "atk-s0404a9:1_8": "khemā upāsikā,",
  "atk-s0404a9:1_9": "rucī upāsikā,",
  "atk-s0404a9:1_10": "cundī rājakumārī,",
  "atk-s0404a9:1_11": "bimbī upāsikā,",
  "atk-s0404a9:1_12": "sumanā rājakumārī,",
  "atk-s0404a9:1_13": "mallikā devī,",
  "atk-s0404a9:1_14": "tissā upāsikā,",
  "atk-s0404a9:1_15": "tissāmātā upāsikā,",
  "atk-s0404a9:1_16": "soṇā upāsikā,",
  "atk-s0404a9:1_17": "soṇāya mātā upāsikā,",
  "atk-s0404a9:1_18": "kāṇā upāsikā,",
  "atk-s0404a9:1_19": "kāṇamātā upāsikā,",
  "atk-s0404a9:1_20": "uttarā nandamātā,",
  "atk-s0404a9:1_21": "visākhā migāramātā,",
  "atk-s0404a9:1_22": "khujjuttarā upāsikā,",
  "atk-s0404a9:1_23": "sāmāvatī upāsikā,",
  "atk-s0404a9:1_24": "suppavāsā koliyadhītā,",
  "atk-s0404a9:1_25": "suppiyā upāsikā,",
  "atk-s0404a9:1_26": "nakulamātā gahapatānīti imāsaṁ ettakānaṁ aṭṭhaṅgasamannāgataṁ uposathakammameva kathitaṁ.",
  "atk-s0404a9:1_27": "Icchantena vitthāretvā kathetabbaṁ.",
  "atk-s0404a9:1_28": "Sesaṁ sabbattha uttānatthamevāti.",
  "atk-s0404a9:2_0": "Manorathapūraṇiyā aṅguttaranikāya-aṭṭhakathāya",
  "atk-s0404a9:3_0": "Aṭṭhakanipātassa saṁvaṇṇanā niṭṭhitā.",
  "atk-s0404a9:4_0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "atk-s0404a9:5_0": "Aṅguttaranikāye",
  "atk-s0404a9:6_0": "Navakanipāta-aṭṭhakathā",
  "atk-s0404a9:7_0": "1. Paṭhamapaṇṇāsakaṁ"
}