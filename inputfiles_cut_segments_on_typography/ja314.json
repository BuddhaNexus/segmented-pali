{
  "ja314:0.1_0": "Jātaka",
  "ja314:0.2_0": "Catukkanipāta",
  "ja314:0.3_0": "Pucimandavagga",
  "ja314:0.4_0": "4. Lohakumbhijātaka",
  "ja314:1.1_0": "“Dujjīvitamajīvimha,",
  "ja314:1.2_0": "ye sante na dadamhase;",
  "ja314:1.3_0": "Vijjamānesu bhogesu,",
  "ja314:1.4_0": "dīpaṁ nākamha attano”.",
  "ja314:2.1_0": "“Saṭṭhi vassasahassāni,",
  "ja314:2.2_0": "paripuṇṇāni sabbaso;",
  "ja314:2.3_0": "Niraye paccamānānaṁ,",
  "ja314:2.4_0": "kadā anto bhavissati”.",
  "ja314:3.1_0": "“Natthi anto kuto anto,",
  "ja314:3.2_0": "na anto paṭidissati;",
  "ja314:3.3_0": "Tadā hi pakataṁ pāpaṁ,",
  "ja314:3.4_0": "mama tuyhañca mārisā”.",
  "ja314:4.1_0": "“Sohaṁ nūna ito gantvā,",
  "ja314:4.2_0": "yoniṁ laddhāna mānusiṁ;",
  "ja314:4.3_0": "Vadaññū sīlasampanno,",
  "ja314:4.4_0": "kāhāmi kusalaṁ bahun”ti.",
  "ja314:5.1_0": "Lohakumbhijātakaṁ catutthaṁ."
}