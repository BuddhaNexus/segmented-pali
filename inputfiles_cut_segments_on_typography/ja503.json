{
  "ja503:0.1_0": "Jātaka",
  "ja503:0.2_0": "Vīsatinipāta",
  "ja503:0.3_0": "Mātaṅgavagga",
  "ja503:0.4_0": "7. Sattigumbajātaka",
  "ja503:1.1_0": "Migaluddo mahārājā,",
  "ja503:1.2_0": "pañcālānaṁ rathesabho;",
  "ja503:1.3_0": "Nikkhanto saha senāya,",
  "ja503:1.4_0": "ogaṇo vanamāgamā.",
  "ja503:2.1_0": "Tatthaddasā araññasmiṁ,",
  "ja503:2.2_0": "takkarānaṁ kuṭiṁ kataṁ;",
  "ja503:2.3_0": "Tassā kuṭiyā nikkhamma,",
  "ja503:2.4_0": "suvo luddāni bhāsati.",
  "ja503:3.1_0": "“Sampannavāhano poso,",
  "ja503:3.2_0": "yuvā sammaṭṭhakuṇḍalo;",
  "ja503:3.3_0": "Sobhati lohituṇhīso,",
  "ja503:3.4_0": "divā sūriyova bhāsati.",
  "ja503:4.1_0": "Majjhanhike sampatike,",
  "ja503:4.2_0": "sutto rājā sasārathi;",
  "ja503:4.3_0": "Handassābharaṇaṁ sabbaṁ,",
  "ja503:4.4_0": "gaṇhāma sāhasā mayaṁ.",
  "ja503:5.1_0": "Nisīthepi raho dāni,",
  "ja503:5.2_0": "Sutto rājā sasārathi;",
  "ja503:5.3_0": "Ādāya vatthaṁ maṇikuṇḍalañca,",
  "ja503:5.4_0": "Hantvāna sākhāhi avattharāma”.",
  "ja503:6.1_0": "“Kiṁ nu ummattarūpova,",
  "ja503:6.2_0": "sattigumba pabhāsasi;",
  "ja503:6.3_0": "Durāsadā hi rājāno,",
  "ja503:6.4_0": "aggi pajjalito yathā”.",
  "ja503:7.1_0": "“Atha tvaṁ patikolamba,",
  "ja503:7.2_0": "matto thullāni gajjasi;",
  "ja503:7.3_0": "Mātari mayhaṁ naggāya,",
  "ja503:7.4_0": "kiṁ nu tvaṁ vijigucchase”.",
  "ja503:8.1_0": "“Uṭṭhehi samma taramāno,",
  "ja503:8.2_0": "rathaṁ yojehi sārathi;",
  "ja503:8.3_0": "Sakuṇo me na ruccati,",
  "ja503:8.4_0": "aññaṁ gacchāma assamaṁ”.",
  "ja503:9.1_0": "“Yutto ratho mahārāja,",
  "ja503:9.2_0": "yutto ca balavāhano;",
  "ja503:9.3_0": "Adhitiṭṭha mahārāja,",
  "ja503:9.4_0": "aññaṁ gacchāma assamaṁ”.",
  "ja503:10.1_0": "“Ko numeva gatā sabbe,",
  "ja503:10.2_0": "ye asmiṁ paricārakā;",
  "ja503:10.3_0": "Esa gacchati pañcālo,",
  "ja503:10.4_0": "mutto tesaṁ adassanā.",
  "ja503:11.1_0": "Kodaṇḍakāni gaṇhatha,",
  "ja503:11.2_0": "sattiyo tomarāni ca;",
  "ja503:11.3_0": "Esa gacchati pañcālo,",
  "ja503:11.4_0": "mā vo muñcittha jīvataṁ”.",
  "ja503:12.1_0": "Athāparo paṭinandittha,",
  "ja503:12.2_0": "suvo lohitatuṇḍako;",
  "ja503:12.3_0": "“Svāgataṁ te mahārāja,",
  "ja503:12.4_0": "atho te adurāgataṁ;",
  "ja503:12.5_0": "Issarosi anuppatto,",
  "ja503:12.6_0": "yaṁ idhatthi pavedaya.",
  "ja503:13.1_0": "Tindukāni piyālāni,",
  "ja503:13.2_0": "madhuke kāsumāriyo;",
  "ja503:13.3_0": "Phalāni khuddakappāni,",
  "ja503:13.4_0": "bhuñja rāja varaṁ varaṁ.",
  "ja503:14.1_0": "Idampi pānīyaṁ sītaṁ,",
  "ja503:14.2_0": "ābhataṁ girigabbharā;",
  "ja503:14.3_0": "Tato piva mahārāja,",
  "ja503:14.4_0": "sace tvaṁ abhikaṅkhasi.",
  "ja503:15.1_0": "Araññaṁ uñchāya gatā,",
  "ja503:15.2_0": "ye asmiṁ paricārakā;",
  "ja503:15.3_0": "Sayaṁ uṭṭhāya gaṇhavho,",
  "ja503:15.4_0": "hatthā me natthi dātave”.",
  "ja503:16.1_0": "“Bhaddako vatayaṁ pakkhī,",
  "ja503:16.2_0": "dijo paramadhammiko;",
  "ja503:16.3_0": "Atheso itaro pakkhī,",
  "ja503:16.4_0": "suvo luddāni bhāsati.",
  "ja503:17.1_0": "‘Etaṁ hanatha bandhatha,",
  "ja503:17.2_0": "Mā vo muñcittha jīvataṁ’;",
  "ja503:17.3_0": "Iccevaṁ vilapantassa,",
  "ja503:17.4_0": "Sotthiṁ pattosmi assamaṁ”.",
  "ja503:18.1_0": "“Bhātarosma mahārāja,",
  "ja503:18.2_0": "sodariyā ekamātukā;",
  "ja503:18.3_0": "Ekarukkhasmiṁ saṁvaḍḍhā,",
  "ja503:18.4_0": "nānākhettagatā ubho.",
  "ja503:19.1_0": "Sattigumbo ca corānaṁ,",
  "ja503:19.2_0": "ahañca isīnaṁ idha;",
  "ja503:19.3_0": "Asataṁ so, sataṁ ahaṁ,",
  "ja503:19.4_0": "tena dhammena no vinā”.",
  "ja503:20.1_0": "“Tattha vadho ca bandho ca,",
  "ja503:20.2_0": "nikatī vañcanāni ca;",
  "ja503:20.3_0": "Ālopā sāhasākārā,",
  "ja503:20.4_0": "tāni so tattha sikkhati.",
  "ja503:21.1_0": "Idha saccañca dhammo ca,",
  "ja503:21.2_0": "Ahiṁsā saṁyamo damo;",
  "ja503:21.3_0": "Āsanūdakadāyīnaṁ,",
  "ja503:21.4_0": "Aṅke vaddhosmi bhāradha”.",
  "ja503:22.1_0": "“Yaṁ yañhi rāja bhajati,",
  "ja503:22.2_0": "santaṁ vā yadi vā asaṁ;",
  "ja503:22.3_0": "Sīlavantaṁ visīlaṁ vā,",
  "ja503:22.4_0": "vasaṁ tasseva gacchati.",
  "ja503:23.1_0": "Yādisaṁ kurute mittaṁ,",
  "ja503:23.2_0": "yādisaṁ cūpasevati;",
  "ja503:23.3_0": "Sopi tādisako hoti,",
  "ja503:23.4_0": "sahavāso hi tādiso.",
  "ja503:24.1_0": "Sevamāno sevamānaṁ,",
  "ja503:24.2_0": "samphuṭṭho samphusaṁ paraṁ;",
  "ja503:24.3_0": "Saro diddho kalāpaṁva,",
  "ja503:24.4_0": "alittamupalimpati;",
  "ja503:24.5_0": "Upalepabhayā dhīro,",
  "ja503:24.6_0": "neva pāpasakhā siyā.",
  "ja503:25.1_0": "Pūtimacchaṁ kusaggena,",
  "ja503:25.2_0": "yo naro upanayhati;",
  "ja503:25.3_0": "Kusāpi pūti vāyanti,",
  "ja503:25.4_0": "evaṁ bālūpasevanā.",
  "ja503:26.1_0": "Tagarañca palāsena,",
  "ja503:26.2_0": "yo naro upanayhati;",
  "ja503:26.3_0": "Pattāpi surabhi vāyanti,",
  "ja503:26.4_0": "evaṁ dhīrūpasevanā.",
  "ja503:27.1_0": "Tasmā pattapuṭasseva,",
  "ja503:27.2_0": "ñatvā sampākamattano;",
  "ja503:27.3_0": "Asante nopaseveyya,",
  "ja503:27.4_0": "sante seveyya paṇḍito;",
  "ja503:27.5_0": "Asanto nirayaṁ nenti,",
  "ja503:27.6_0": "santo pāpenti suggatin”ti.",
  "ja503:28.1_0": "Sattigumbajātakaṁ sattamaṁ."
}