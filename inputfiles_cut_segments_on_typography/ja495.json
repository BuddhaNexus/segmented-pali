{
  "ja495:0.1_0": "Jātaka",
  "ja495:0.2_0": "Pakiṇṇakanipāta",
  "ja495:0.3_0": "Sālikedāravagga",
  "ja495:0.4_0": "12. Dasabrāhmaṇajātaka",
  "ja495:1.1_0": "Rājā avoca vidhuraṁ,",
  "ja495:1.2_0": "Dhammakāmo yudhiṭṭhilo;",
  "ja495:1.3_0": "“Brāhmaṇe vidhura pariyesa,",
  "ja495:1.4_0": "Sīlavante bahussute.",
  "ja495:2.1_0": "Virate methunā dhammā,",
  "ja495:2.2_0": "Ye me bhuñjeyyu bhojanaṁ;",
  "ja495:2.3_0": "Dakkhiṇaṁ samma dassāma,",
  "ja495:2.4_0": "Yattha dinnaṁ mahapphalaṁ”.",
  "ja495:3.1_0": "“Dullabhā brāhmaṇā deva,",
  "ja495:3.2_0": "sīlavanto bahussutā;",
  "ja495:3.3_0": "Viratā methunā dhammā,",
  "ja495:3.4_0": "ye te bhuñjeyyu bhojanaṁ.",
  "ja495:4.1_0": "Dasa khalu mahārāja,",
  "ja495:4.2_0": "yā tā brāhmaṇajātiyo;",
  "ja495:4.3_0": "Tesaṁ vibhaṅgaṁ vicayaṁ,",
  "ja495:4.4_0": "vitthārena suṇohi me.",
  "ja495:5.1_0": "Pasibbake gahetvāna,",
  "ja495:5.2_0": "puṇṇe mūlassa saṁvute;",
  "ja495:5.3_0": "Osadhikāyo ganthenti,",
  "ja495:5.4_0": "nhāpayanti japanti ca.",
  "ja495:6.1_0": "Tikicchakasamā rāja,",
  "ja495:6.2_0": "tepi vuccanti brāhmaṇā;",
  "ja495:6.3_0": "Akkhātā te mahārāja,",
  "ja495:6.4_0": "tādise nipatāmase”.",
  "ja495:7.1_0": "“Apetā te ca brahmaññā,",
  "ja495:7.2_0": "(iti rājā korabyo)",
  "ja495:7.3_0": "Na te vuccanti brāhmaṇā;",
  "ja495:7.4_0": "Aññe vidhura pariyesa,",
  "ja495:7.5_0": "Sīlavante bahussute.",
  "ja495:8.1_0": "Virate methunā dhammā,",
  "ja495:8.2_0": "Ye me bhuñjeyyu bhojanaṁ;",
  "ja495:8.3_0": "Dakkhiṇaṁ samma dassāma,",
  "ja495:8.4_0": "Yattha dinnaṁ mahapphalaṁ”.",
  "ja495:9.1_0": "“Kiṅkiṇikāyo gahetvā,",
  "ja495:9.2_0": "ghosenti puratopi te;",
  "ja495:9.3_0": "Pesanānipi gacchanti,",
  "ja495:9.4_0": "rathacariyāsu sikkhare.",
  "ja495:10.1_0": "Paricārakasamā rāja,",
  "ja495:10.2_0": "tepi vuccanti brāhmaṇā;",
  "ja495:10.3_0": "Akkhātā te mahārāja,",
  "ja495:10.4_0": "tādise nipatāmase”.",
  "ja495:11.1_0": "“Apetā te ca brahmaññā,",
  "ja495:11.2_0": "(iti rājā korabyo)",
  "ja495:11.3_0": "Na te vuccanti brāhmaṇā;",
  "ja495:11.4_0": "Aññe vidhura pariyesa,",
  "ja495:11.5_0": "Sīlavante bahussute.",
  "ja495:12.1_0": "Virate methunā dhammā,",
  "ja495:12.2_0": "ye me bhuñjeyyu bhojanaṁ;",
  "ja495:12.3_0": "Dakkhiṇaṁ samma dassāma,",
  "ja495:12.4_0": "yattha dinnaṁ mahapphalaṁ”.",
  "ja495:13.1_0": "“Kamaṇḍaluṁ gahetvāna,",
  "ja495:13.2_0": "vaṅkadaṇḍañca brāhmaṇā;",
  "ja495:13.3_0": "Paccupessanti rājāno,",
  "ja495:13.4_0": "gāmesu nigamesu ca;",
  "ja495:13.5_0": "Nādinne vuṭṭhahissāma,",
  "ja495:13.6_0": "gāmamhi vā vanamhi vā.",
  "ja495:14.1_0": "Niggāhakasamā rāja,",
  "ja495:14.2_0": "tepi vuccanti brāhmaṇā;",
  "ja495:14.3_0": "Akkhātā te mahārāja,",
  "ja495:14.4_0": "tādise nipatāmase”.",
  "ja495:15.1_0": "“Apetā te ca brahmaññā,",
  "ja495:15.2_0": "(iti rājā korabyo)",
  "ja495:15.3_0": "Na te vuccanti brāhmaṇā;",
  "ja495:15.4_0": "Aññe vidhura pariyesa,",
  "ja495:15.5_0": "Sīlavante bahussute.",
  "ja495:16.1_0": "Virate methunā dhammā,",
  "ja495:16.2_0": "ye me bhuñjeyyu bhojanaṁ;",
  "ja495:16.3_0": "Dakkhiṇaṁ samma dassāma,",
  "ja495:16.4_0": "yattha dinnaṁ mahapphalaṁ”.",
  "ja495:17.1_0": "“Parūḷhakacchanakhalomā,",
  "ja495:17.2_0": "paṅkadantā rajassirā;",
  "ja495:17.3_0": "Okiṇṇā rajareṇūhi,",
  "ja495:17.4_0": "yācakā vicaranti te.",
  "ja495:18.1_0": "Khāṇughātasamā rāja,",
  "ja495:18.2_0": "tepi vuccanti brāhmaṇā;",
  "ja495:18.3_0": "Akkhātā te mahārāja,",
  "ja495:18.4_0": "tādise nipatāmase”.",
  "ja495:19.1_0": "“Apetā te ca brahmaññā,",
  "ja495:19.2_0": "(iti rājā korabyo)",
  "ja495:19.3_0": "Na te vuccanti brāhmaṇā;",
  "ja495:19.4_0": "Aññe vidhura pariyesa,",
  "ja495:19.5_0": "Sīlavante bahussute.",
  "ja495:20.1_0": "Virate methunā dhammā,",
  "ja495:20.2_0": "ye me bhuñjeyyu bhojanaṁ;",
  "ja495:20.3_0": "Dakkhiṇaṁ samma dassāma,",
  "ja495:20.4_0": "yattha dinnaṁ mahapphalaṁ”.",
  "ja495:21.1_0": "“Harītakaṁ āmalakaṁ,",
  "ja495:21.2_0": "Ambaṁ jambuṁ vibhītakaṁ;",
  "ja495:21.3_0": "Labujaṁ dantapoṇāni,",
  "ja495:21.4_0": "Beluvā badarāni ca.",
  "ja495:22.1_0": "Rājāyatanaṁ ucchupuṭaṁ,",
  "ja495:22.2_0": "dhūmanettaṁ madhuañjanaṁ;",
  "ja495:22.3_0": "Uccāvacāni paṇiyāni,",
  "ja495:22.4_0": "vipaṇenti janādhipa.",
  "ja495:23.1_0": "Vāṇijakasamā rāja,",
  "ja495:23.2_0": "tepi vuccanti brāhmaṇā;",
  "ja495:23.3_0": "Akkhātā te mahārāja,",
  "ja495:23.4_0": "tādise nipatāmase”.",
  "ja495:24.1_0": "“Apetā te ca brahmaññā,",
  "ja495:24.2_0": "(iti rājā korabyo)",
  "ja495:24.3_0": "Na te vuccanti brāhmaṇā;",
  "ja495:24.4_0": "Aññe vidhura pariyesa,",
  "ja495:24.5_0": "Sīlavante bahussute.",
  "ja495:25.1_0": "Virate methunā dhammā,",
  "ja495:25.2_0": "ye me bhuñjeyyu bhojanaṁ;",
  "ja495:25.3_0": "Dakkhiṇaṁ samma dassāma,",
  "ja495:25.4_0": "yattha dinnaṁ mahapphalaṁ”.",
  "ja495:26.1_0": "“Kasivāṇijjaṁ kārenti,",
  "ja495:26.2_0": "posayanti ajeḷake;",
  "ja495:26.3_0": "Kumāriyo pavecchanti,",
  "ja495:26.4_0": "vivāhantāvahanti ca.",
  "ja495:27.1_0": "Samā ambaṭṭhavessehi,",
  "ja495:27.2_0": "tepi vuccanti brāhmaṇā;",
  "ja495:27.3_0": "Akkhātā te mahārāja,",
  "ja495:27.4_0": "tādise nipatāmase”.",
  "ja495:28.1_0": "“Apetā te ca brahmaññā,",
  "ja495:28.2_0": "(iti rājā korabyo)",
  "ja495:28.3_0": "Na te vuccanti brāhmaṇā;",
  "ja495:28.4_0": "Aññe vidhura pariyesa,",
  "ja495:28.5_0": "Sīlavante bahussute.",
  "ja495:29.1_0": "Virate methunā dhammā,",
  "ja495:29.2_0": "ye me bhuñjeyyu bhojanaṁ;",
  "ja495:29.3_0": "Dakkhiṇaṁ samma dassāma,",
  "ja495:29.4_0": "yattha dinnaṁ mahapphalaṁ”.",
  "ja495:30.1_0": "“Nikkhittabhikkhaṁ bhuñjanti,",
  "ja495:30.2_0": "gāmesveke purohitā;",
  "ja495:30.3_0": "Bahū te paripucchanti,",
  "ja495:30.4_0": "aṇḍacchedā nilañchakā.",
  "ja495:31.1_0": "Pasūpi tattha haññanti,",
  "ja495:31.2_0": "mahiṁsā sūkarā ajā;",
  "ja495:31.3_0": "Goghātakasamā rāja,",
  "ja495:31.4_0": "tepi vuccanti brāhmaṇā;",
  "ja495:31.5_0": "Akkhātā te mahārāja,",
  "ja495:31.6_0": "tādise nipatāmase”.",
  "ja495:32.1_0": "“Apetā te ca brahmaññā,",
  "ja495:32.2_0": "(iti rājā korabyo)",
  "ja495:32.3_0": "Na te vuccanti brāhmaṇā;",
  "ja495:32.4_0": "Aññe vidhura pariyesa,",
  "ja495:32.5_0": "Sīlavante bahussute.",
  "ja495:33.1_0": "Virate methunā dhammā,",
  "ja495:33.2_0": "ye me bhuñjeyyu bhojanaṁ;",
  "ja495:33.3_0": "Dakkhiṇaṁ samma dassāma,",
  "ja495:33.4_0": "yattha dinnaṁ mahapphalaṁ”.",
  "ja495:34.1_0": "“Asicammaṁ gahetvāna,",
  "ja495:34.2_0": "khaggaṁ paggayha brāhmaṇā;",
  "ja495:34.3_0": "Vessapathesu tiṭṭhanti,",
  "ja495:34.4_0": "satthaṁ abbāhayantipi.",
  "ja495:35.1_0": "Samā gopanisādehi,",
  "ja495:35.2_0": "tepi vuccanti brāhmaṇā;",
  "ja495:35.3_0": "Akkhātā te mahārāja,",
  "ja495:35.4_0": "tādise nipatāmase”.",
  "ja495:36.1_0": "“Apetā te ca brahmaññā,",
  "ja495:36.2_0": "(iti rājā korabyo)",
  "ja495:36.3_0": "Na te vuccanti brāhmaṇā;",
  "ja495:36.4_0": "Aññe vidhura pariyesa,",
  "ja495:36.5_0": "Sīlavante bahussute.",
  "ja495:37.1_0": "Virate methunā dhammā,",
  "ja495:37.2_0": "ye me bhuñjeyyu bhojanaṁ;",
  "ja495:37.3_0": "Dakkhiṇaṁ samma dassāma,",
  "ja495:37.4_0": "yattha dinnaṁ mahapphalaṁ”.",
  "ja495:38.1_0": "“Araññe kuṭikaṁ katvā,",
  "ja495:38.2_0": "kūṭāni kārayanti te;",
  "ja495:38.3_0": "Sasabiḷāre bādhenti,",
  "ja495:38.4_0": "āgodhā macchakacchapaṁ.",
  "ja495:39.1_0": "Te luddakasamā rāja,",
  "ja495:39.2_0": "tepi vuccanti brāhmaṇā;",
  "ja495:39.3_0": "Akkhātā te mahārāja,",
  "ja495:39.4_0": "tādise nipatāmase”.",
  "ja495:40.1_0": "“Apetā te ca brahmaññā,",
  "ja495:40.2_0": "(iti rājā korabyo)",
  "ja495:40.3_0": "Na te vuccanti brāhmaṇā;",
  "ja495:40.4_0": "Aññe vidhura pariyesa,",
  "ja495:40.5_0": "Sīlavante bahussute.",
  "ja495:41.1_0": "Virate methunā dhammā,",
  "ja495:41.2_0": "ye me bhuñjeyyu bhojanaṁ;",
  "ja495:41.3_0": "Dakkhiṇaṁ samma dassāma,",
  "ja495:41.4_0": "yattha dinnaṁ mahapphalaṁ”.",
  "ja495:42.1_0": "“Aññe dhanassa kāmā hi,",
  "ja495:42.2_0": "heṭṭhāmañce pasakkitā;",
  "ja495:42.3_0": "Rājāno upari nhāyanti,",
  "ja495:42.4_0": "somayāge upaṭṭhite.",
  "ja495:43.1_0": "Malamajjakasamā rāja,",
  "ja495:43.2_0": "Tepi vuccanti brāhmaṇā;",
  "ja495:43.3_0": "Akkhātā te mahārāja,",
  "ja495:43.4_0": "Tādise nipatāmase”.",
  "ja495:44.1_0": "“Apetā te ca brahmaññā,",
  "ja495:44.2_0": "(iti rājā korabyo)",
  "ja495:44.3_0": "Na te vuccanti brāhmaṇā;",
  "ja495:44.4_0": "Aññe vidhura pariyesa,",
  "ja495:44.5_0": "Sīlavante bahussute.",
  "ja495:45.1_0": "Virate methunā dhammā,",
  "ja495:45.2_0": "ye me bhuñjeyyu bhojanaṁ;",
  "ja495:45.3_0": "Dakkhiṇaṁ samma dassāma,",
  "ja495:45.4_0": "yattha dinnaṁ mahapphalaṁ”.",
  "ja495:46.1_0": "“Atthi kho brāhmaṇā deva,",
  "ja495:46.2_0": "sīlavanto bahussutā;",
  "ja495:46.3_0": "Viratā methunā dhammā,",
  "ja495:46.4_0": "ye te bhuñjeyyu bhojanaṁ.",
  "ja495:47.1_0": "Ekañca bhattaṁ bhuñjanti,",
  "ja495:47.2_0": "na ca majjaṁ pivanti te;",
  "ja495:47.3_0": "Akkhātā te mahārāja,",
  "ja495:47.4_0": "tādise nipatāmase”.",
  "ja495:48.1_0": "“Ete kho brāhmaṇā vidhura,",
  "ja495:48.2_0": "Sīlavanto bahussutā;",
  "ja495:48.3_0": "Ete vidhura pariyesa,",
  "ja495:48.4_0": "Khippañca ne nimantayā”ti.",
  "ja495:49.1_0": "Dasabrāhmaṇajātakaṁ dvādasamaṁ."
}