{
  "tika-s0403t6:0_0": "(7) 2. Saññāvaggo",
  "tika-s0403t6:1_0": "1-5. Saññāsuttādivaṇṇanā",
  "tika-s0403t6:2_0": "61- 65.",
  "tika-s0403t6:2_1": "Dutiyassa paṭhame “mahapphalā mahānisaṁsā”ti ubhayampetaṁ atthato ekaṁ,",
  "tika-s0403t6:2_2": "byañjanameva nānanti āha “mahapphalā”tiādi.",
  "tika-s0403t6:2_3": "“Pañcime gahapatayo ānisaṁsā”tiādīsu ānisaṁsa-",
  "tika-s0403t6:2_4": "saddo phalapariyāyopi hoti.",
  "tika-s0403t6:2_5": "Mahato lokuttarassa sukhassa paccayā hontīti mahānisaṁsā.",
  "tika-s0403t6:2_6": "Amatogadhāti amatabbhantarā amataṁ anuppaviṭṭhā nibbānadiṭṭhattā,",
  "tika-s0403t6:2_7": "tato paraṁ na gacchanti.",
  "tika-s0403t6:2_8": "Tena vuttaṁ “amatapariyosānā”ti.",
  "tika-s0403t6:2_9": "Amataṁ pariyosānaṁ avasānaṁ etāsanti amatapariyosānā.",
  "tika-s0403t6:2_10": "Maraṇasaññāti maraṇānupassanāñāṇena saññā.",
  "tika-s0403t6:2_11": "Āhāre paṭikūlasaññāti āhāraṁ gamanādivasena paṭikūlato pariggaṇhantassa uppannasaññā.",
  "tika-s0403t6:2_12": "Ukkaṇṭhitassāti nibbindantassa katthacipi asajjantassa.",
  "tika-s0403t6:2_13": "Dutiyādīni uttānatthāneva.",
  "tika-s0403t6:3_0": "Saññāsuttādivaṇṇanā niṭṭhitā.",
  "tika-s0403t6:4_0": "6-10. Sājīvasuttādivaṇṇanā",
  "tika-s0403t6:5_0": "66- 70.",
  "tika-s0403t6:5_1": "Chaṭṭhe saha ājīvanti etthāti sājīvo,",
  "tika-s0403t6:5_2": "pañhassa pucchanaṁ vissajjanañca.",
  "tika-s0403t6:5_3": "Tenāha “sājīvoti pañhapucchanañceva pañhavissajjanañcā”tiādi.",
  "tika-s0403t6:5_4": "Abhisaṅkhatanti citaṁ.",
  "tika-s0403t6:5_5": "Sattamādīni uttānatthāneva.",
  "tika-s0403t6:6_0": "Sājīvasuttādivaṇṇanā niṭṭhitā.",
  "tika-s0403t6:7_0": "Saññāvaggavaṇṇanā niṭṭhitā."
}