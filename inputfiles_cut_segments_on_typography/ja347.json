{
  "ja347:0.1_0": "Jātaka",
  "ja347:0.2_0": "Catukkanipāta",
  "ja347:0.3_0": "Cūḷakuṇālavagga",
  "ja347:0.4_0": "7. Ayakūṭajātaka",
  "ja347:1.1_0": "“Sabbāyasaṁ kūṭamatippamāṇaṁ,",
  "ja347:1.2_0": "Paggayha yo tiṭṭhasi antalikkhe;",
  "ja347:1.3_0": "Rakkhāya me tvaṁ vihito nusajja,",
  "ja347:1.4_0": "Udāhu me cetayase vadhāya”.",
  "ja347:2.1_0": "“Dūto ahaṁ rājidha rakkhasānaṁ,",
  "ja347:2.2_0": "Vadhāya tuyhaṁ pahitohamasmi;",
  "ja347:2.3_0": "Indo ca taṁ rakkhati devarājā,",
  "ja347:2.4_0": "Tenuttamaṅgaṁ na te phālayāmi”.",
  "ja347:3.1_0": "“Sace ca maṁ rakkhati devarājā,",
  "ja347:3.2_0": "Devānamindo maghavā sujampati;",
  "ja347:3.3_0": "Kāmaṁ pisācā vinadantu sabbe,",
  "ja347:3.4_0": "Na santase rakkhasiyā pajāya.",
  "ja347:4.1_0": "Kāmaṁ kandantu kumbhaṇḍā,",
  "ja347:4.2_0": "Sabbe paṁsupisācakā;",
  "ja347:4.3_0": "Nālaṁ pisācā yuddhāya,",
  "ja347:4.4_0": "Mahatī sā vibhiṁsikā”ti.",
  "ja347:5.1_0": "Ayakūṭajātakaṁ sattamaṁ."
}