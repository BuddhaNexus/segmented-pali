{
  "vv1:0.1_0": "Vimānavatthu",
  "vv1:0.2_0": "Itthivimāna",
  "vv1:0.3_0": "Pīṭhavagga",
  "vv1:0.4_0": "1. Paṭhamapīṭhavimānavatthu",
  "vv1:1.1_0": "Namo tassa Bhagavato Arahato Sammāsambuddhassa.",
  "vv1:2.1_0": "“Pīṭhaṁ te sovaṇṇamayaṁ uḷāraṁ,",
  "vv1:2.2_0": "Manojavaṁ gacchati yenakāmaṁ;",
  "vv1:2.3_0": "Alaṅkate malyadhare suvatthe,",
  "vv1:2.4_0": "Obhāsasi vijjurivabbhakūṭaṁ.",
  "vv1:3.1_0": "Kena tetādiso vaṇṇo,",
  "vv1:3.2_0": "kena te idha mijjhati;",
  "vv1:3.3_0": "Uppajjanti ca te bhogā,",
  "vv1:3.4_0": "ye keci manaso piyā.",
  "vv1:4.1_0": "Pucchāmi taṁ devi mahānubhāve,",
  "vv1:4.2_0": "Manussabhūtā kimakāsi puññaṁ;",
  "vv1:4.3_0": "Kenāsi evaṁ jalitānubhāvā,",
  "vv1:4.4_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv1:5.1_0": "Sā devatā attamanā,",
  "vv1:5.2_0": "moggallānena pucchitā;",
  "vv1:5.3_0": "Pañhaṁ puṭṭhā viyākāsi,",
  "vv1:5.4_0": "yassa kammassidaṁ phalaṁ.",
  "vv1:6.1_0": "“Ahaṁ manussesu manussabhūtā,",
  "vv1:6.2_0": "Abbhāgatānāsanakaṁ adāsiṁ;",
  "vv1:6.3_0": "Abhivādayiṁ añjalikaṁ akāsiṁ,",
  "vv1:6.4_0": "Yathānubhāvañca adāsi dānaṁ.",
  "vv1:7.1_0": "Tena metādiso vaṇṇo,",
  "vv1:7.2_0": "tena me idha mijjhati;",
  "vv1:7.3_0": "Uppajjanti ca me bhogā,",
  "vv1:7.4_0": "ye keci manaso piyā.",
  "vv1:8.1_0": "Akkhāmi te bhikkhu mahānubhāva,",
  "vv1:8.2_0": "Manussabhūtā yamakāsi puññaṁ;",
  "vv1:8.3_0": "Tenamhi evaṁ jalitānubhāvā,",
  "vv1:8.4_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv1:9.1_0": "Paṭhamapīṭhavimānaṁ paṭhamaṁ."
}