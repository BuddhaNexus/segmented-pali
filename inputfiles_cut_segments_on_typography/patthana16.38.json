{
  "patthana16.38:0.1_0": "Paṭṭhānapakaraṇa",
  "patthana16.38:0.2_0": "Dhammānulomapaccanīya (3), Tikadukapaṭṭhānapāḷi (4)",
  "patthana16.38:0.3_0": "16.38. Hīnattika, Hetuduka",
  "patthana16.38:1.1_0": "Hīnaṁ hetuṁ dhammaṁ paṭicca nahīno nahetu dhammo uppajjati hetupaccayā.",
  "patthana16.38:1.1_1": "(Saṅkhittaṁ.) Hetuyā terasa,",
  "patthana16.38:1.1_2": "ārammaṇe nava …pe…",
  "patthana16.38:1.1_3": "vipāke aṭṭha …pe…",
  "patthana16.38:1.1_4": "avigate terasa.",
  "patthana16.38:2.1_0": "Hīnaṁ nahetuṁ dhammaṁ paṭicca namajjhimo nanahetu dhammo uppajjati hetupaccayā.",
  "patthana16.38:2.1_1": "(Saṅkhittaṁ.) Hetuyā nava …pe…",
  "patthana16.38:2.1_2": "vipāke cha …pe…",
  "patthana16.38:2.1_3": "avigate nava."
}