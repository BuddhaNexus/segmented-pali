{
  "ja84:0.1_0": "Jātaka",
  "ja84:0.2_0": "Ekakanipāta",
  "ja84:0.3_0": "Apāyimhavagga",
  "ja84:0.4_0": "4. Atthassadvārajātaka",
  "ja84:1.1_0": "“Ārogyamicche paramañca lābhaṁ,",
  "ja84:1.2_0": "Sīlañca vuddhānumataṁ sutañca;",
  "ja84:1.3_0": "Dhammānuvattī ca alīnatā ca,",
  "ja84:1.4_0": "Atthassa dvārā pamukhā chaḷete”ti.",
  "ja84:2.1_0": "Atthassadvārajātakaṁ catutthaṁ."
}