{
  "patthana17.13:0.1_0": "Paṭṭhānapakaraṇa",
  "patthana17.13:0.2_0": "Dhammānulomapaccanīya (3), Tikatikapaṭṭhānapāḷi (5)",
  "patthana17.13:0.3_0": "17.13. Kusalattika, Hīnattika",
  "patthana17.13:1.1_0": "Akusalaṁ hīnaṁ dhammaṁ paṭicca naakusalo nahīno dhammo uppajjati hetupaccayā.",
  "patthana17.13:1.1_1": "(Saṅkhittaṁ.) Hetuyā tīṇi,",
  "patthana17.13:1.1_2": "adhipatiyā tīṇi …pe…",
  "patthana17.13:1.1_3": "avigate tīṇi.",
  "patthana17.13:2.1_0": "Abyākataṁ majjhimaṁ dhammaṁ paccayā naabyākato namajjhimo dhammo uppajjati hetupaccayā.",
  "patthana17.13:2.1_1": "(Saṅkhittaṁ.) Hetuyā cha,",
  "patthana17.13:2.1_2": "ārammaṇe cha …pe…",
  "patthana17.13:2.1_3": "āsevane pañca …pe…",
  "patthana17.13:2.1_4": "vipāke tīṇi …pe…",
  "patthana17.13:2.1_5": "avigate cha.",
  "patthana17.13:3.1_0": "Kusalaṁ paṇītaṁ dhammaṁ paṭicca nakusalo napaṇīto dhammo uppajjati hetupaccayā.",
  "patthana17.13:3.1_1": "(Saṅkhittaṁ.) Hetuyā cha,",
  "patthana17.13:3.1_2": "adhipatiyā cha …pe…",
  "patthana17.13:3.1_3": "vipāke tīṇi …pe…",
  "patthana17.13:3.1_4": "avigate cha."
}