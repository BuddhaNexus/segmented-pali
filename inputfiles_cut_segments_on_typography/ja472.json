{
  "ja472:0.1_0": "Jātaka",
  "ja472:0.2_0": "Dvādasakanipāta",
  "ja472:0.3_0": "Cūḷakuṇālavagga",
  "ja472:0.4_0": "9. Mahāpadumajātaka",
  "ja472:1.1_0": "“Nādaṭṭhā parato dosaṁ,",
  "ja472:1.2_0": "aṇuṁthūlāni sabbaso;",
  "ja472:1.3_0": "Issaro paṇaye daṇḍaṁ,",
  "ja472:1.4_0": "sāmaṁ appaṭivekkhiya.",
  "ja472:2.1_0": "Yo ca appaṭivekkhitvā,",
  "ja472:2.2_0": "daṇḍaṁ kubbati khattiyo;",
  "ja472:2.3_0": "Sakaṇṭakaṁ so gilati,",
  "ja472:2.4_0": "jaccandhova samakkhikaṁ.",
  "ja472:3.1_0": "Adaṇḍiyaṁ daṇḍayati,",
  "ja472:3.2_0": "daṇḍiyañca adaṇḍiyaṁ;",
  "ja472:3.3_0": "Andhova visamaṁ maggaṁ,",
  "ja472:3.4_0": "na jānāti samāsamaṁ.",
  "ja472:4.1_0": "Yo ca etāni ṭhānāni,",
  "ja472:4.2_0": "Aṇuṁthūlāni sabbaso;",
  "ja472:4.3_0": "Sudiṭṭhamanusāseyya,",
  "ja472:4.4_0": "Sa ve voharitumarahati.",
  "ja472:5.1_0": "Nekantamudunā sakkā,",
  "ja472:5.2_0": "Ekantatikhiṇena vā;",
  "ja472:5.3_0": "Attaṁ mahante ṭhapetuṁ,",
  "ja472:5.4_0": "Tasmā ubhayamācare.",
  "ja472:6.1_0": "Paribhūto mudu hoti,",
  "ja472:6.2_0": "atitikkho ca veravā;",
  "ja472:6.3_0": "Etañca ubhayaṁ ñatvā,",
  "ja472:6.4_0": "anumajjhaṁ samācare.",
  "ja472:7.1_0": "Bahumpi ratto bhāseyya,",
  "ja472:7.2_0": "duṭṭhopi bahu bhāsati;",
  "ja472:7.3_0": "Na itthikāraṇā rāja,",
  "ja472:7.4_0": "puttaṁ ghātetumarahasi”.",
  "ja472:8.1_0": "“Sabbova loko ekato,",
  "ja472:8.2_0": "Itthī ca ayamekikā;",
  "ja472:8.3_0": "Tenāhaṁ paṭipajjissaṁ,",
  "ja472:8.4_0": "Gacchatha pakkhipatheva taṁ”.",
  "ja472:9.1_0": "“Anekatāle narake,",
  "ja472:9.2_0": "gambhīre ca suduttare;",
  "ja472:9.3_0": "Pātito giriduggasmiṁ,",
  "ja472:9.4_0": "kena tvaṁ tattha nāmari”.",
  "ja472:10.1_0": "“Nāgo jātaphaṇo tattha,",
  "ja472:10.2_0": "thāmavā girisānujo;",
  "ja472:10.3_0": "Paccaggahi maṁ bhogehi,",
  "ja472:10.4_0": "tenāhaṁ tattha nāmariṁ”.",
  "ja472:11.1_0": "“Ehi taṁ paṭinessāmi,",
  "ja472:11.2_0": "rājaputta sakaṁ gharaṁ;",
  "ja472:11.3_0": "Rajjaṁ kārehi bhaddante,",
  "ja472:11.4_0": "kiṁ araññe karissasi”.",
  "ja472:12.1_0": "“Yathā gilitvā baḷisaṁ,",
  "ja472:12.2_0": "Uddhareyya salohitaṁ;",
  "ja472:12.3_0": "Uddharitvā sukhī assa,",
  "ja472:12.4_0": "Evaṁ passāmi attanaṁ”.",
  "ja472:13.1_0": "“Kiṁ nu tvaṁ baḷisaṁ brūsi,",
  "ja472:13.2_0": "kiṁ tvaṁ brūsi salohitaṁ;",
  "ja472:13.3_0": "Kiṁ nu tvaṁ ubbhataṁ brūsi,",
  "ja472:13.4_0": "taṁ me akkhāhi pucchito”.",
  "ja472:14.1_0": "“Kāmāhaṁ baḷisaṁ brūmi,",
  "ja472:14.2_0": "hatthiassaṁ salohitaṁ;",
  "ja472:14.3_0": "Cattāhaṁ ubbhataṁ brūmi,",
  "ja472:14.4_0": "evaṁ jānāhi khattiya”.",
  "ja472:15.1_0": "“Ciñcamāṇavikā mātā,",
  "ja472:15.2_0": "devadatto ca me pitā;",
  "ja472:15.3_0": "Ānando paṇḍito nāgo,",
  "ja472:15.4_0": "sāriputto ca devatā;",
  "ja472:15.5_0": "Rājaputto ahaṁ āsiṁ,",
  "ja472:15.6_0": "evaṁ dhāretha jātakan”ti.",
  "ja472:16.1_0": "Mahāpadumajātakaṁ navamaṁ."
}