{
  "tika-abh02t27:0_0": "10. Bojjhaṅgavibhaṅgo",
  "tika-abh02t27:1_0": "1. Suttantabhājanīyaṁ",
  "tika-abh02t27:2_0": "Paṭhamanayavaṇṇanā",
  "tika-abh02t27:3_0": "466.",
  "tika-abh02t27:3_1": "Patiṭṭhānaṁ idha saṁsāre avaṭṭhānaṁ,",
  "tika-abh02t27:3_2": "tassa mūlaṁ kilesāti āha “kilesavasena patiṭṭhāna”nti.",
  "tika-abh02t27:3_3": "Patiṭṭhānāya pana byāpārāpatti kammanti vuttaṁ “abhisaṅkhāravasena āyūhanā”ti.",
  "tika-abh02t27:3_4": "Yasmā kilesesu taṇhādiṭṭhiyo taṇhādiṭṭhicaritānaṁ visesato saṁsāranāyikā,",
  "tika-abh02t27:3_5": "kilesasahitameva ca kammaṁ patiṭṭhānāya hoti,",
  "tika-abh02t27:3_6": "na kevalaṁ,",
  "tika-abh02t27:3_7": "tasmā vuttaṁ “taṇhādiṭṭhīhi…pe…",
  "tika-abh02t27:3_8": "āyūhanā”ti.",
  "tika-abh02t27:3_9": "Tathā taṇhāya bhavassādabhāvato,",
  "tika-abh02t27:3_10": "diṭṭhiyā vibhavābhinandanabhūtāya vibhavābhisaṅkharaṇabhāvato “taṇhāvasena patiṭṭhānaṁ,",
  "tika-abh02t27:3_11": "diṭṭhivasena āyūhanā”ti vuttaṁ.",
  "tika-abh02t27:3_12": "Diṭṭhīsupi antomukhappavattāya bhavadiṭṭhiyā visesato saṁsāre avaṭṭhānaṁ,",
  "tika-abh02t27:3_13": "yato olīyanāti vuccatīti āha “sassatadiṭṭhiyā patiṭṭhāna”nti.",
  "tika-abh02t27:3_14": "Bahimukhappavattāpi vibhavadiṭṭhi bhavābhisaṅkharaṇaṁ nātivattatīti vuttaṁ “ucchedadiṭṭhiyā āyūhanā”ti.",
  "tika-abh02t27:3_15": "Layāpatti yathāraddhassa ārambhassa aniṭṭhānaṁ antosaṅkocabhāvatoti āha “līnavasena patiṭṭhāna”nti.",
  "tika-abh02t27:3_16": "Uddhatāpatti anupāyabhūtā byāpārāpatti asaṅkocabhāvatoti vuttaṁ “uddhaccavasena āyūhanā”ti.",
  "tika-abh02t27:3_17": "Tathā kosajjapakkhikattā ca kāmasukhānuyogassa uddhaccapakkhikattā ca attakilamathānuyogassa tadubhayavasena patiṭṭhānāyūhanā vuttā,",
  "tika-abh02t27:3_18": "itaraṁ vuttanayānusārena veditabbaṁ.",
  "tika-abh02t27:3_19": "Idhāti imissā sammohavinodaniyā.",
  "tika-abh02t27:3_20": "Avuttānanti “kilesavasena patiṭṭhāna”ntiādīnaṁ vasena veditabbā patiṭṭhānāyūhanāti yojanā.",
  "tika-abh02t27:4_0": "Samappavatte dhammeti līnuddhaccavirahena samappavatte sampayuttadhamme.",
  "tika-abh02t27:4_1": "Paṭisañcikkhatīti patirūpaṁ saṅkaleti gaṇeti tuleti.",
  "tika-abh02t27:4_2": "Tenāha “upapattito ikkhatī”ti.",
  "tika-abh02t27:4_3": "Tadākāroti paṭisaṅkhānākāro upapattito ikkhanākāro.",
  "tika-abh02t27:4_4": "Evañca katvāti paṭisaṅkhānasabhāvattā eva upekkhāsambojjhaṅgassa.",
  "tika-abh02t27:4_5": "Pacchimapacchimakāraṇabhāvoti pacchimassa pacchimassa kāraṇabhāvo.",
  "tika-abh02t27:4_6": "Purimaṁ purimañhi pacchimassa pacchimassa visesapaccayoti.",
  "tika-abh02t27:5_0": "467.",
  "tika-abh02t27:5_1": "Aviparītakāyādisabhāvaggahaṇasamatthatāya balavatī eva sati.",
  "tika-abh02t27:5_2": "Paññā gahitā satinepakkenāti attho.",
  "tika-abh02t27:5_3": "Evaṁcittoti evaṁ lābhasakkārasilokasannissitacitto.",
  "tika-abh02t27:5_4": "Cirakatavattādivasenāti cirakatavattādisīsena.",
  "tika-abh02t27:5_5": "“Vutto”ti imināpi “katvā āha kāyaviññattiṁ…pe…",
  "tika-abh02t27:5_6": "koṭṭhāsa”nti yojanā.",
  "tika-abh02t27:6_0": "Paresanti na anantarānaṁ.",
  "tika-abh02t27:6_1": "Sabbesaṁ…pe…",
  "tika-abh02t27:6_2": "yojetabbā “sabbe bojjhaṅgā sabbesaṁ paccayavisesā hontiyevā”ti.",
  "tika-abh02t27:6_3": "Kāmetīti kāmo,",
  "tika-abh02t27:6_4": "assādanavasena āmasatīti āmisaṁ,",
  "tika-abh02t27:6_5": "kāmova āmisanti kāmāmisaṁ,",
  "tika-abh02t27:6_6": "kilesakāmo.",
  "tika-abh02t27:6_7": "Vatthukāmo pana āmasīyatīti āmisaṁ.",
  "tika-abh02t27:6_8": "Evaṁ sesadvayampi.",
  "tika-abh02t27:6_9": "Tesu lokīyanti ettha sukhavisesāti loko,",
  "tika-abh02t27:6_10": "upapattiviseso.",
  "tika-abh02t27:6_11": "Vaṭṭaṁ saṁsāro.",
  "tika-abh02t27:6_12": "Kāmassādavasena pavatto lobho kāmāmisaṁ.",
  "tika-abh02t27:6_13": "Bhavavisesapatthanāvasena pavatto lokāmisaṁ.",
  "tika-abh02t27:6_14": "Vibhavo nāma kimatthiyo,",
  "tika-abh02t27:6_15": "ko vā taṁ abhipattheyyāti vaṭṭānugedhabhūto lobho vaṭṭāmisanti ca vadanti.",
  "tika-abh02t27:6_16": "Tadārammaṇanti tassā taṇhāya ārammaṇaṁ, rūpādi.",
  "tika-abh02t27:6_17": "Lokadhammā lābhādayo.",
  "tika-abh02t27:6_18": "Vuttāvasesā sabbāva taṇhā saṁsārajanako rāgo.",
  "tika-abh02t27:7_0": "Paṭhamanayavaṇṇanā niṭṭhitā.",
  "tika-abh02t27:8_0": "Dutiyanayavaṇṇanā",
  "tika-abh02t27:9_0": "468-",
  "tika-abh02t27:9_1": "9.Sabbe sattāti kāmabhavādīsu,",
  "tika-abh02t27:9_2": "saññībhavādīsu,",
  "tika-abh02t27:9_3": "ekavokārabhavādīsu ca sabbabhavesu sabbe sattā.",
  "tika-abh02t27:9_4": "Āhārato ṭhiti etesanti āhāraṭṭhitikā,",
  "tika-abh02t27:9_5": "paccayaṭṭhitikā.",
  "tika-abh02t27:9_6": "Yena paccayena te tiṭṭhanti,",
  "tika-abh02t27:9_7": "so ekova dhammo ñātapariññāsaṅkhātāya “āhāraṭṭhitikā”ti abhiññāya abhiññeyyo.",
  "tika-abh02t27:9_8": "Dve dhātuyoti saṅkhatāsaṅkhatadhātuyo.",
  "tika-abh02t27:9_9": "Tisso dhātuyoti kāmadhāturūpadhātuarūpadhātuyo.",
  "tika-abh02t27:9_10": "Pañca vimuttāyatanānīti “idha,",
  "tika-abh02t27:9_11": "bhikkhave,",
  "tika-abh02t27:9_12": "bhikkhuno satthā dhammaṁ deseti aññataro vā garuṭṭhāniyo”tiādinā āgatāni vimuccanakāraṇāni.",
  "tika-abh02t27:9_13": "Anuttariyānīti dassanānuttariyādīni cha anuttariyāni.",
  "tika-abh02t27:9_14": "Niddasavatthūnīti yehi kāraṇehi niddaso hoti,",
  "tika-abh02t27:9_15": "tāni niddasavatthūni nāma.",
  "tika-abh02t27:9_16": "Desanāmattañcetaṁ.",
  "tika-abh02t27:9_17": "Khīṇāsavo hi dasavasso hutvā parinibbuto puna dasavasso na hoti.",
  "tika-abh02t27:9_18": "Na kevalañca dasavasso,",
  "tika-abh02t27:9_19": "navavassopi…pe…",
  "tika-abh02t27:9_20": "ekamuhuttikopi na hotiyeva puna paṭisandhiyā abhāvā,",
  "tika-abh02t27:9_21": "aṭṭhuppattivasena panevaṁ vuttaṁ.",
  "tika-abh02t27:9_22": "Tāni pana “idha, bhikkhu,",
  "tika-abh02t27:9_23": "sikkhāsamādāne tibbacchando hotī”tiādinā sutte āgatāniyeva.",
  "tika-abh02t27:9_24": "“Sammādiṭṭhissa purisapuggalassa micchādiṭṭhi nijjiṇṇā hotī”tiādīni dasa nijjaravatthūni.",
  "tika-abh02t27:9_25": "Khandhādayoti khandhāyatanadhātādayo.",
  "tika-abh02t27:9_26": "Vinandhananti “bhavādivinandhanaṭṭhena vānaṁ vuccati taṇhā”ti dasseti.",
  "tika-abh02t27:9_27": "Gamananti assādanavasena ārammaṇe pavattimāha.",
  "tika-abh02t27:9_28": "Tena vuttaṁ “piyarūpasātarūpesū”ti.",
  "tika-abh02t27:10_0": "Vipassanāsahagatanti veditabbaṁ viññattisamuṭṭhāpakattā.",
  "tika-abh02t27:10_1": "“Maggaṁ appattaṁ kāyikaṁ vīriya”nti visesetvā vuttattā pana lokuttaravīriyampi pariyāyena kāyikaṁ nāma atthīti dīpitaṁ hoti.",
  "tika-abh02t27:11_0": "Rūpāvacare pītisambojjhaṅgoti na vuccatīti āha “rūpāvacare…pe…",
  "tika-abh02t27:11_1": "paṭikkhittā”ti.",
  "tika-abh02t27:11_2": "Yathā vipassanāsahagatā pīti pariyāyena “pītisambojjhaṅgo”ti vuccati,",
  "tika-abh02t27:11_3": "evaṁ rūpāvacare pīti nibbedhabhāgiyā vattabbā siyā.",
  "tika-abh02t27:11_4": "Evaṁ labbhamānāpi alabbhamānaṁ upādāya na vuttā.",
  "tika-abh02t27:11_5": "“Avitakkaavicārā”ti visesanaṁ santapaṇītāya pītiyā dassanatthaṁ.",
  "tika-abh02t27:11_6": "Bojjhaṅgabhūtāti pariyāyabojjhaṅgabhūtā.",
  "tika-abh02t27:11_7": "Avitakkaavicāro pīti…pe…",
  "tika-abh02t27:11_8": "na vutto savitakkasavicārattā tassa.",
  "tika-abh02t27:11_9": "Na hi kāmāvacarā avitakkaavicārā pīti atthi.",
  "tika-abh02t27:12_0": "Idha vutto pariyāyenāti attho.",
  "tika-abh02t27:12_1": "Maggapaṭivedhānulomanato vipassanāya viya pādakajjhānesupi satiādayo “bojjhaṅgā”tveva vuccantīti āha “nibbedhabhāgiyattā na paṭikkhipitabbo”ti.",
  "tika-abh02t27:12_2": "Evaṁ kasiṇajjhānādīsu bojjhaṅge uddharantānaṁ adhippāyaṁ vatvā anuddharantānaṁ adhippāyaṁ vattuṁ “anuddharantā panā”tiādimāha.",
  "tika-abh02t27:12_3": "Te hi āsannekantakiccanibbattīhi vipassanākkhaṇe bojjhaṅge uddharanti,",
  "tika-abh02t27:12_4": "na jhānakkhaṇe tadabhāvato.",
  "tika-abh02t27:12_5": "Tenāha “vipassanākiccassa viya…pe…",
  "tika-abh02t27:12_6": "na uddharantī”ti.",
  "tika-abh02t27:12_7": "Kasiṇanissando arūpānīti āha “tadāyattānī”ti.",
  "tika-abh02t27:13_0": "Dutiyanayavaṇṇanā niṭṭhitā.",
  "tika-abh02t27:14_0": "Tatiyanayavaṇṇanā",
  "tika-abh02t27:15_0": "470- 1.",
  "tika-abh02t27:15_1": "Vossajjanaṁ pahānaṁ vossaggo,",
  "tika-abh02t27:15_2": "vossajjanaṁ vā vissaṭṭhabhāvo nirāsaṅkānuppavesoti āha “vossaggasaddo…pe…",
  "tika-abh02t27:15_3": "duvidhatā vuttā”ti.",
  "tika-abh02t27:15_4": "Vipassanākkhaṇe tadaṅgatanninnappakārena,",
  "tika-abh02t27:15_5": "maggakkhaṇe samucchedatadārammaṇakaraṇappakārena.",
  "tika-abh02t27:16_0": "Tatiyanayavaṇṇanā niṭṭhitā.",
  "tika-abh02t27:17_0": "Suttantabhājanīyavaṇṇanā niṭṭhitā.",
  "tika-abh02t27:18_0": "2. Abhidhammabhājanīyavaṇṇanā",
  "tika-abh02t27:19_0": "472.",
  "tika-abh02t27:19_1": "Upekkhanamupekkhāsambojjhaṅgassa sabhāvo,",
  "tika-abh02t27:19_2": "so ca samādhivīriyasambojjhaṅgo viya sampayuttānaṁ ūnādhikabhāvabyāvaṭo ahutvā tesaṁ anūnānadhikabhāve",
  "tika-abh02t27:19_3": "majjhattākārappavattīti imamatthaṁ āha “upekkhanavasenā”tiādinā.",
  "tika-abh02t27:19_4": "Tattha upapattito ikkhananti paṭisaṅkhānamāha.",
  "tika-abh02t27:20_0": "Abhidhammabhājanīyavaṇṇanā niṭṭhitā.",
  "tika-abh02t27:21_0": "Bojjhaṅgavibhaṅgavaṇṇanā niṭṭhitā."
}