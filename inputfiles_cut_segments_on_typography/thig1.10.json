{
  "thig1.10:0.1_0": "Therīgāthā 1.10",
  "thig1.10:0.2_0": "Ekakanipāta",
  "thig1.10:0.3_0": "Upasamātherīgāthā",
  "thig1.10:1.1_0": "“Upasame tare oghaṁ,",
  "thig1.10:1.2_0": "maccudheyyaṁ suduttaraṁ;",
  "thig1.10:1.3_0": "Dhārehi antimaṁ dehaṁ,",
  "thig1.10:1.4_0": "jetvā māraṁ savāhinin”ti.",
  "thig1.10:2.1_0": "… Upasamā therī …."
}