{
  "ja250:0.1_0": "Jātaka",
  "ja250:0.2_0": "Dukanipāta",
  "ja250:0.3_0": "Siṅgālavagga",
  "ja250:0.4_0": "10. Kapijātaka",
  "ja250:1.1_0": "“Ayaṁ isī upasamasaṁyame rato,",
  "ja250:1.2_0": "Sa tiṭṭhati sisirabhayena aṭṭito;",
  "ja250:1.3_0": "Handa ayaṁ pavisatumaṁ agārakaṁ,",
  "ja250:1.4_0": "Vinetu sītaṁ darathañca kevalaṁ”.",
  "ja250:2.1_0": "“Nāyaṁ isī upasamasaṁyame rato,",
  "ja250:2.2_0": "Kapī ayaṁ dumavarasākhagocaro;",
  "ja250:2.3_0": "So dūsako rosako cāpi jammo,",
  "ja250:2.4_0": "Sacevajemampi dūseyyagāran”ti.",
  "ja250:3.1_0": "Kapijātakaṁ dasamaṁ.",
  "ja250:3.2_0": "Siṅgālavaggo dasamo.",
  "ja250:4.1_0": "Tassuddānaṁ",
  "ja250:5.1_0": "Atha rājā siṅgālavaro sunakho,",
  "ja250:5.2_0": "Tathā kosiya icchati kālaghaso;",
  "ja250:5.3_0": "Atha dānavaroṭṭhapi sārathinā,",
  "ja250:5.4_0": "Punambavanañca sisirakapi dasāti.",
  "ja250:6.1_0": "Atha vagguddānaṁ",
  "ja250:7.1_0": "Daḷhañca vaggaṁ aparena santhavaṁ,",
  "ja250:7.2_0": "Kalyāṇavaggāsadiso ca rūhakaṁ;",
  "ja250:7.3_0": "Nataṁdaḷha bīraṇathambhakaṁ puna,",
  "ja250:7.4_0": "Kāsāvupāhana siṅgālakena dasāti.",
  "ja250:8.1_0": "Dukanipātaṁ niṭṭhitaṁ."
}