{
  "tika-abh08t20:0_0": "21. Ekavīsatimo paricchedo",
  "tika-abh08t20:1_0": "Paṭipadāñāṇadassanavisuddhiniddesavaṇṇanā",
  "tika-abh08t20:2_0": "1299.Upallesavimuttaṁsuvisadaṁ ñāṇanti aṭṭhavidhañāṇaṁ sandhāyāha.",
  "tika-abh08t20:3_0": "1302. Saccānulomañāṇanti ayameva pavuccati.",
  "tika-abh08t20:4_0": "1306. Ayaṁ sikhāpattā vipassanā eva saccānulomañāṇaṁ nāma pavuccati.",
  "tika-abh08t20:5_0": "1309.Bhavaṅgānantaraṁ saṅkhāru-",
  "tika-abh08t20:5_1": "pekkhāgatanayenāti bhavaṅgānantaraṁ saṅkhārupekkhāsaṅkhātena ñāṇena katanayena.",
  "tika-abh08t20:6_0": "1314-",
  "tika-abh08t20:6_1": "6.Bodhipakkhiyadhammānaṁ,",
  "tika-abh08t20:6_2": "uddhañca anulomatoti uddhaṁ vattamānānaṁ bodhipakkhiyadhammānaṁ anulomato,",
  "tika-abh08t20:6_3": "idaṁ anulomañāṇaṁ vuṭṭhānagāminiyā pariyosānaṁ nāma bhāsitaṁ mahesinā kathitaṁ.",
  "tika-abh08t20:6_4": "Saccānulomañāṇaṁ āhacca uppannā,",
  "tika-abh08t20:6_5": "sabbappakārena gotrabhupariyosānaṁ.",
  "tika-abh08t20:7_0": "1318.Tatthāti vipassanāya.",
  "tika-abh08t20:8_0": "Iti abhidhammāvatāraṭīkāya",
  "tika-abh08t20:9_0": "Paṭipadāñāṇadassanavisuddhiniddesavaṇṇanā niṭṭhitā.",
  "tika-abh08t20:10_0": "Ekavīsatimo paricchedo."
}