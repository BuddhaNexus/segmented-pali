{
  "ne28:0.1_0": "Netti",
  "ne28:0.2_0": "Paṭiniddesavāra",
  "ne28:0.3_0": "Sampāta 9",
  "ne28:0.4_0": "Parivattanahārasampāta",
  "ne28:1.1_0": "Tattha katamo parivattano hārasampāto?",
  "ne28:2.1_0": "“Tasmā rakkhitacittassa, sammāsaṅkappagocaro”ti gāthā.",
  "ne28:2.2_0": "Samathavipassanāya bhāvitāya nirodho phalaṁ,",
  "ne28:2.2_1": "pariññātaṁ dukkhaṁ,",
  "ne28:2.2_2": "samudayo pahīno,",
  "ne28:2.2_3": "maggo bhāvito paṭipakkhena.",
  "ne28:2.3_0": "Niyutto parivattano hārasampāto."
}