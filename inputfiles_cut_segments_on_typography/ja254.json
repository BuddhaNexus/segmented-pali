{
  "ja254:0.1_0": "Jātaka",
  "ja254:0.2_0": "Tikanipāta",
  "ja254:0.3_0": "Saṅkappavagga",
  "ja254:0.4_0": "4. Kuṇḍakakucchisindhavajātaka",
  "ja254:1.1_0": "“Bhutvā tiṇaparighāsaṁ,",
  "ja254:1.2_0": "bhutvā ācāmakuṇḍakaṁ;",
  "ja254:1.3_0": "Etaṁ te bhojanaṁ āsi,",
  "ja254:1.4_0": "kasmā dāni na bhuñjasi”.",
  "ja254:2.1_0": "“Yattha posaṁ na jānanti,",
  "ja254:2.2_0": "jātiyā vinayena vā;",
  "ja254:2.3_0": "Bahuṁ tattha mahābrahme,",
  "ja254:2.4_0": "api ācāmakuṇḍakaṁ.",
  "ja254:3.1_0": "Tvañca kho maṁ pajānāsi,",
  "ja254:3.2_0": "yādisāyaṁ hayuttamo;",
  "ja254:3.3_0": "Jānanto jānamāgamma,",
  "ja254:3.4_0": "na te bhakkhāmi kuṇḍakan”ti.",
  "ja254:4.1_0": "Kuṇḍakakucchisindhavajātakaṁ catutthaṁ."
}