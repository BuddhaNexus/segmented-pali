{
  "thag1.38:0.1_0": "Theragāthā 1.38",
  "thag1.38:0.2_0": "Ekakanipāta",
  "thag1.38:0.3_0": "Catutthavagga",
  "thag1.38:0.4_0": "Gavampatittheragāthā",
  "thag1.38:1.1_0": "“Yo iddhiyā sarabhuṁ aṭṭhapesi,",
  "thag1.38:1.2_0": "So gavampati asito anejo;",
  "thag1.38:1.3_0": "Taṁ sabbasaṅgātigataṁ mahāmuniṁ,",
  "thag1.38:1.4_0": "Devā namassanti bhavassa pāragun”ti.",
  "thag1.38:1.5_0": "… Gavampatitthero …."
}