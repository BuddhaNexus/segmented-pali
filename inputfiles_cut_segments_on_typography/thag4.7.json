{
  "thag4.7:0.1_0": "Theragāthā 4.7",
  "thag4.7:0.2_0": "Catukkanipāta",
  "thag4.7:0.3_0": "Paṭhamavagga",
  "thag4.7:0.4_0": "Sambhūtattheragāthā",
  "thag4.7:1.1_0": "“Yo dandhakāle tarati,",
  "thag4.7:1.2_0": "taraṇīye ca dandhaye;",
  "thag4.7:1.3_0": "Ayoni saṁvidhānena,",
  "thag4.7:1.4_0": "bālo dukkhaṁ nigacchati.",
  "thag4.7:2.1_0": "Tassatthā parihāyanti,",
  "thag4.7:2.2_0": "kāḷapakkheva candimā;",
  "thag4.7:2.3_0": "Āyasakyañca pappoti,",
  "thag4.7:2.4_0": "mittehi ca virujjhati.",
  "thag4.7:3.1_0": "Yo dandhakāle dandheti,",
  "thag4.7:3.2_0": "taraṇīye ca tāraye;",
  "thag4.7:3.3_0": "Yoniso saṁvidhānena,",
  "thag4.7:3.4_0": "sukhaṁ pappoti paṇḍito.",
  "thag4.7:4.1_0": "Tassatthā paripūrenti,",
  "thag4.7:4.2_0": "sukkapakkheva candimā;",
  "thag4.7:4.3_0": "Yaso kittiñca pappoti,",
  "thag4.7:4.4_0": "mittehi na virujjhatī”ti.",
  "thag4.7:4.5_0": "… Sambhūto thero …."
}