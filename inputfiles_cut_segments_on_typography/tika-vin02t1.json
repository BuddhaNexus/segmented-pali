{
  "tika-vin02t1:0_0": "6. Pāṭidesanīyakaṇḍaṁ",
  "tika-vin02t1:1_0": "Pāṭidesanīyasikkhāpadavaṇṇanā",
  "tika-vin02t1:2_0": "552.",
  "tika-vin02t1:2_1": "Pāṭidesanīyesu paṭhame “gārayhaṁ āvusotiādi paṭidesetabbākāradassana”nti vacanato pāḷiyaṁ āgatanayeneva āpatti desetabbā.",
  "tika-vin02t1:2_2": "Asappāyanti saggamokkhānaṁ ahitaṁ ananukūlaṁ.",
  "tika-vin02t1:2_3": "Sesamettha uttānameva.",
  "tika-vin02t1:2_4": "Paripuṇṇūpasampannatā,",
  "tika-vin02t1:2_5": "aññātikatā,",
  "tika-vin02t1:2_6": "antaraghare ṭhitāya hatthato sahatthā paṭiggahaṇaṁ,",
  "tika-vin02t1:2_7": "yāvakālikatā,",
  "tika-vin02t1:2_8": "ajjhoharaṇanti imāni panettha pañca aṅgāni.",
  "tika-vin02t1:3_0": "557- 562.",
  "tika-vin02t1:3_1": "Dutiyatatiyacatutthesu natthi vattabbaṁ,",
  "tika-vin02t1:3_2": "aṅgesu pana dutiye paripuṇṇūpasampannatā,",
  "tika-vin02t1:3_3": "pañcabhojanatā,",
  "tika-vin02t1:3_4": "antaraghare ṭhitāya anuññātappakārato aññathā vosāsanā,",
  "tika-vin02t1:3_5": "anivāraṇā,",
  "tika-vin02t1:3_6": "ajjhohāroti imāni pañca aṅgāni.",
  "tika-vin02t1:4_0": "Tatiye sekkhasammatatā,",
  "tika-vin02t1:4_1": "pubbe animantitatā,",
  "tika-vin02t1:4_2": "agilānatā,",
  "tika-vin02t1:4_3": "gharūpacārokkamanaṁ,",
  "tika-vin02t1:4_4": "ṭhapetvā niccabhattādīni aññaṁ āmisaṁ gahetvā bhuñjananti imāni pañca aṅgāni.",
  "tika-vin02t1:5_0": "Catutthe yathāvuttaāraññakasenāsanatā,",
  "tika-vin02t1:5_1": "yāvakālikassa atatthajātakatā,",
  "tika-vin02t1:5_2": "agilānatā,",
  "tika-vin02t1:5_3": "agilānāvasesakatā,",
  "tika-vin02t1:5_4": "appaṭisaṁviditatā,",
  "tika-vin02t1:5_5": "ajjhārāme paṭiggahaṇaṁ,",
  "tika-vin02t1:5_6": "ajjhoharaṇanti imāni satta aṅgāni.",
  "tika-vin02t1:6_0": "Pāṭidesanīyasikkhāpadavaṇṇanā niṭṭhitā.",
  "tika-vin02t1:7_0": "Pāṭidesanīyakaṇḍaṁ niṭṭhitaṁ."
}