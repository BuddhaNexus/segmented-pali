{
  "ja214:0.1_0": "Jātaka",
  "ja214:0.2_0": "Dukanipāta",
  "ja214:0.3_0": "Bīraṇathambhavagga",
  "ja214:0.4_0": "4. Puṇṇanadījātaka",
  "ja214:1.1_0": "“Puṇṇaṁ nadiṁ yena ca peyyamāhu,",
  "ja214:1.2_0": "Jātaṁ yavaṁ yena ca guyhamāhu;",
  "ja214:1.3_0": "Dūraṁ gataṁ yena ca avhayanti,",
  "ja214:1.4_0": "So tyāgato handa ca bhuñja brāhmaṇa”.",
  "ja214:2.1_0": "“Yato maṁ saratī rājā,",
  "ja214:2.2_0": "vāyasampi pahetave;",
  "ja214:2.3_0": "Haṁsā koñcā mayūrā ca,",
  "ja214:2.4_0": "asatīyeva pāpiyā”ti.",
  "ja214:3.1_0": "Puṇṇanadījātakaṁ catutthaṁ."
}