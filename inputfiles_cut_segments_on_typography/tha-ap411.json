{
  "tha-ap411:0.1_0": "Therāpadāna",
  "tha-ap411:0.2_0": "Metteyyavagga",
  "tha-ap411:0.3_0": "9. Jatukaṇṇittheraapadāna",
  "tha-ap411:1.1_0": "“Nagare haṁsavatiyā,",
  "tha-ap411:1.2_0": "seṭṭhiputto ahosahaṁ;",
  "tha-ap411:1.3_0": "Samappito kāmaguṇe,",
  "tha-ap411:1.4_0": "paricāremahaṁ tadā.",
  "tha-ap411:2.1_0": "Tato pāsādamāruyha,",
  "tha-ap411:2.2_0": "mahābhoge valañjako;",
  "tha-ap411:2.3_0": "Tattha naccehi gītehi,",
  "tha-ap411:2.4_0": "paricāremahaṁ tadā.",
  "tha-ap411:3.1_0": "Tūriyā āhatā mayhaṁ,",
  "tha-ap411:3.2_0": "sammatāḷasamāhitā;",
  "tha-ap411:3.3_0": "Naccantā itthiyo sabbā,",
  "tha-ap411:3.4_0": "harantiyeva me mano.",
  "tha-ap411:4.1_0": "Celāpikā vāmanikā,",
  "tha-ap411:4.2_0": "kuñjavāsī timajjhikā;",
  "tha-ap411:4.3_0": "Laṅghikā sokajjhāyī ca,",
  "tha-ap411:4.4_0": "parivārenti maṁ sadā.",
  "tha-ap411:5.1_0": "Vetāḷino kumbhathūnī,",
  "tha-ap411:5.2_0": "naṭā ca naccakā bahū;",
  "tha-ap411:5.3_0": "Naṭakā nāṭakā ceva,",
  "tha-ap411:5.4_0": "parivārenti maṁ sadā.",
  "tha-ap411:6.1_0": "Kappakā nhāpakā sūdā,",
  "tha-ap411:6.2_0": "mālākārā supāsakā;",
  "tha-ap411:6.3_0": "Jallā mallā ca te sabbe,",
  "tha-ap411:6.4_0": "parivārenti maṁ sadā.",
  "tha-ap411:7.1_0": "Etesu kīḷamānesu,",
  "tha-ap411:7.2_0": "sikkhite katupāsane;",
  "tha-ap411:7.3_0": "Rattindivaṁ na jānāmi,",
  "tha-ap411:7.4_0": "indova tidasaṅgaṇe.",
  "tha-ap411:8.1_0": "Addhikā pathikā sabbe,",
  "tha-ap411:8.2_0": "yācakā varakā bahū;",
  "tha-ap411:8.3_0": "Upagacchanti te niccaṁ,",
  "tha-ap411:8.4_0": "bhikkhayantā mamaṁ gharaṁ.",
  "tha-ap411:9.1_0": "Samaṇā brāhmaṇā ceva,",
  "tha-ap411:9.2_0": "puññakkhettā anuttarā;",
  "tha-ap411:9.3_0": "Vaḍḍhayantā mamaṁ puññaṁ,",
  "tha-ap411:9.4_0": "āgacchanti mamaṁ gharaṁ.",
  "tha-ap411:10.1_0": "Paṭagā laṭukā sabbe,",
  "tha-ap411:10.2_0": "nigaṇṭhā pupphasāṭakā;",
  "tha-ap411:10.3_0": "Tedaṇḍikā ekasikhā,",
  "tha-ap411:10.4_0": "āgacchanti mamaṁ gharaṁ.",
  "tha-ap411:11.1_0": "Ājīvakā viluttāvī,",
  "tha-ap411:11.2_0": "godhammā devadhammikā;",
  "tha-ap411:11.3_0": "Rajojalladharā ete,",
  "tha-ap411:11.4_0": "āgacchanti mamaṁ gharaṁ.",
  "tha-ap411:12.1_0": "Parittakā santipattā,",
  "tha-ap411:12.2_0": "kodhapugganikā bahū;",
  "tha-ap411:12.3_0": "Tapassī vanacārī ca,",
  "tha-ap411:12.4_0": "āgacchanti mamaṁ gharaṁ.",
  "tha-ap411:13.1_0": "Oḍḍakā damiḷā ceva,",
  "tha-ap411:13.2_0": "sākuḷā malavāḷakā;",
  "tha-ap411:13.3_0": "Savarā yonakā ceva,",
  "tha-ap411:13.4_0": "āgacchanti mamaṁ gharaṁ.",
  "tha-ap411:14.1_0": "Andhakā muṇḍakā sabbe,",
  "tha-ap411:14.2_0": "koṭalā hanuvindakā;",
  "tha-ap411:14.3_0": "Ārāvacīnaraṭṭhā ca,",
  "tha-ap411:14.4_0": "āgacchanti mamaṁ gharaṁ.",
  "tha-ap411:15.1_0": "Alasandakā pallavakā,",
  "tha-ap411:15.2_0": "dhammarā niggamānusā;",
  "tha-ap411:15.3_0": "Gehikā cetaputtā ca,",
  "tha-ap411:15.4_0": "āgacchanti mamaṁ gharaṁ.",
  "tha-ap411:16.1_0": "Mādhurakā kosalakā,",
  "tha-ap411:16.2_0": "kaliṅgā hatthiporikā;",
  "tha-ap411:16.3_0": "Isiṇḍā makkalā ceva,",
  "tha-ap411:16.4_0": "āgacchanti mamaṁ gharaṁ.",
  "tha-ap411:17.1_0": "Celāvakā ārabbhā ca,",
  "tha-ap411:17.2_0": "oghuḷhā meghalā bahū;",
  "tha-ap411:17.3_0": "Khuddakā suddakā ceva,",
  "tha-ap411:17.4_0": "āgacchanti mamaṁ gharaṁ.",
  "tha-ap411:18.1_0": "Rohaṇā sindhavā ceva,",
  "tha-ap411:18.2_0": "citakā ekakaṇṇikā;",
  "tha-ap411:18.3_0": "Suraṭṭhā aparantā ca,",
  "tha-ap411:18.4_0": "āgacchanti mamaṁ gharaṁ.",
  "tha-ap411:19.1_0": "Suppārakā kumārā ca,",
  "tha-ap411:19.2_0": "mallasovaṇṇabhūmikā;",
  "tha-ap411:19.3_0": "Vajjītaṅgā ca te sabbe,",
  "tha-ap411:19.4_0": "āgacchanti mamaṁ gharaṁ.",
  "tha-ap411:20.1_0": "Naḷakārā pesakārā,",
  "tha-ap411:20.2_0": "cammakārā ca tacchakā;",
  "tha-ap411:20.3_0": "Kammārā kumbhakārā ca,",
  "tha-ap411:20.4_0": "āgacchanti mamaṁ gharaṁ.",
  "tha-ap411:21.1_0": "Maṇikārā lohakārā,",
  "tha-ap411:21.2_0": "soṇṇakārā ca dussikā;",
  "tha-ap411:21.3_0": "Tipukārā ca te sabbe,",
  "tha-ap411:21.4_0": "āgacchanti mamaṁ gharaṁ.",
  "tha-ap411:22.1_0": "Usukārā bhamakārā,",
  "tha-ap411:22.2_0": "pesakārā ca gandhikā;",
  "tha-ap411:22.3_0": "Rajakā tunnavāyā ca,",
  "tha-ap411:22.4_0": "āgacchanti mamaṁ gharaṁ.",
  "tha-ap411:23.1_0": "Telikā kaṭṭhahārā ca,",
  "tha-ap411:23.2_0": "udahārā ca pessikā;",
  "tha-ap411:23.3_0": "Sūpikā sūparakkhā ca,",
  "tha-ap411:23.4_0": "āgacchanti mamaṁ gharaṁ.",
  "tha-ap411:24.1_0": "Dovārikā anīkaṭṭhā,",
  "tha-ap411:24.2_0": "Bandhikā pupphachaḍḍakā;",
  "tha-ap411:24.3_0": "Hatthāruhā hatthipālā,",
  "tha-ap411:24.4_0": "Āgacchanti mamaṁ gharaṁ.",
  "tha-ap411:25.1_0": "Ānandassa mahārañño,",
  "tha-ap411:25.2_0": "mamatthassa adāsahaṁ;",
  "tha-ap411:25.3_0": "Sattavaṇṇena ratanena,",
  "tha-ap411:25.4_0": "ūnatthaṁ pūrayāmahaṁ.",
  "tha-ap411:26.1_0": "Ye mayā kittitā sabbe,",
  "tha-ap411:26.2_0": "nānāvaṇṇā bahū janā;",
  "tha-ap411:26.3_0": "Tesāhaṁ cittamaññāya,",
  "tha-ap411:26.4_0": "tappayiṁ ratanenahaṁ.",
  "tha-ap411:27.1_0": "Vaggūsu bhāsamānāsu,",
  "tha-ap411:27.2_0": "vajjamānāsu bherisu;",
  "tha-ap411:27.3_0": "Saṅkhesu dhamayantesu,",
  "tha-ap411:27.4_0": "sakagehe ramāmahaṁ.",
  "tha-ap411:28.1_0": "Bhagavā tamhi samaye,",
  "tha-ap411:28.2_0": "padumuttaranāyako;",
  "tha-ap411:28.3_0": "Vasīsatasahassehi,",
  "tha-ap411:28.4_0": "parikkhīṇāsavehi so.",
  "tha-ap411:29.1_0": "Bhikkhūhi sahito vīthiṁ,",
  "tha-ap411:29.2_0": "paṭipajjittha cakkhumā;",
  "tha-ap411:29.3_0": "Obhāsento disā sabbā,",
  "tha-ap411:29.4_0": "dīparukkhova jotati.",
  "tha-ap411:30.1_0": "Vajjanti bheriyo sabbā,",
  "tha-ap411:30.2_0": "gacchante lokanāyake;",
  "tha-ap411:30.3_0": "Pabhā niddhāvate tassa,",
  "tha-ap411:30.4_0": "sataraṁsīva uggato.",
  "tha-ap411:31.1_0": "Kavāṭantarikāyāpi,",
  "tha-ap411:31.2_0": "paviṭṭhena ca rasminā;",
  "tha-ap411:31.3_0": "Antogharesu vipulo,",
  "tha-ap411:31.4_0": "āloko āsi tāvade.",
  "tha-ap411:32.1_0": "Pabhaṁ disvāna buddhassa,",
  "tha-ap411:32.2_0": "pārisajje avocahaṁ;",
  "tha-ap411:32.3_0": "‘Nissaṁsayaṁ buddhaseṭṭho,",
  "tha-ap411:32.4_0": "imaṁ vīthimupāgato’.",
  "tha-ap411:33.1_0": "Khippaṁ oruyha pāsādā,",
  "tha-ap411:33.2_0": "agamiṁ antarāpaṇaṁ;",
  "tha-ap411:33.3_0": "Sambuddhaṁ abhivādetvā,",
  "tha-ap411:33.4_0": "idaṁ vacanamabraviṁ.",
  "tha-ap411:34.1_0": "‘Anukampatu me buddho,",
  "tha-ap411:34.2_0": "jalajuttamanāyako;",
  "tha-ap411:34.3_0": "Vasīsatasahassehi,",
  "tha-ap411:34.4_0": "adhivāsesi so muni’.",
  "tha-ap411:35.1_0": "Nimantetvāna sambuddhaṁ,",
  "tha-ap411:35.2_0": "abhinesiṁ sakaṁ gharaṁ;",
  "tha-ap411:35.3_0": "Tattha annena pānena,",
  "tha-ap411:35.4_0": "santappesiṁ mahāmuniṁ.",
  "tha-ap411:36.1_0": "Bhuttāviṁ kālamaññāya,",
  "tha-ap411:36.2_0": "buddhaseṭṭhassa tādino;",
  "tha-ap411:36.3_0": "Sataṅgikena tūriyena,",
  "tha-ap411:36.4_0": "buddhaseṭṭhaṁ upaṭṭhahiṁ.",
  "tha-ap411:37.1_0": "Padumuttaro lokavidū,",
  "tha-ap411:37.2_0": "āhutīnaṁ paṭiggaho;",
  "tha-ap411:37.3_0": "Antoghare nisīditvā,",
  "tha-ap411:37.4_0": "imā gāthā abhāsatha.",
  "tha-ap411:38.1_0": "‘Yo maṁ tūriyehupaṭṭhāsi,",
  "tha-ap411:38.2_0": "annapānañcadāsi me;",
  "tha-ap411:38.3_0": "Tamahaṁ kittayissāmi,",
  "tha-ap411:38.4_0": "suṇātha mama bhāsato.",
  "tha-ap411:39.1_0": "Pahūtabhakkho hutvāna,",
  "tha-ap411:39.2_0": "sahirañño sabhojano;",
  "tha-ap411:39.3_0": "Catudīpe ekarajjaṁ,",
  "tha-ap411:39.4_0": "kārayissatiyaṁ naro.",
  "tha-ap411:40.1_0": "Pañca sīle samādāya,",
  "tha-ap411:40.2_0": "dasakammapathe tato;",
  "tha-ap411:40.3_0": "Samādāya pavattento,",
  "tha-ap411:40.4_0": "parisaṁ sikkhāpayissati.",
  "tha-ap411:41.1_0": "Tūriyasatasahassāni,",
  "tha-ap411:41.2_0": "bheriyo samalaṅkatā;",
  "tha-ap411:41.3_0": "Vajjayissantimaṁ niccaṁ,",
  "tha-ap411:41.4_0": "upaṭṭhānassidaṁ phalaṁ.",
  "tha-ap411:42.1_0": "Tiṁsakappasahassāni,",
  "tha-ap411:42.2_0": "devaloke ramissati;",
  "tha-ap411:42.3_0": "Catusaṭṭhikkhattuṁ devindo,",
  "tha-ap411:42.4_0": "devarajjaṁ karissati.",
  "tha-ap411:43.1_0": "Catusaṭṭhikkhattuṁ rājā,",
  "tha-ap411:43.2_0": "cakkavattī bhavissati;",
  "tha-ap411:43.3_0": "Padesarajjaṁ vipulaṁ,",
  "tha-ap411:43.4_0": "gaṇanāto asaṅkhiyaṁ.",
  "tha-ap411:44.1_0": "Kappasatasahassamhi,",
  "tha-ap411:44.2_0": "okkākakulasambhavo;",
  "tha-ap411:44.3_0": "Gotamo nāma gottena,",
  "tha-ap411:44.4_0": "satthā loke bhavissati.",
  "tha-ap411:45.1_0": "Upapajjati yaṁ yoniṁ,",
  "tha-ap411:45.2_0": "devattaṁ atha mānusaṁ;",
  "tha-ap411:45.3_0": "Anūnabhogo hutvāna,",
  "tha-ap411:45.4_0": "manussattaṁ gamissati.",
  "tha-ap411:46.1_0": "Ajjhāyako bhavitvāna,",
  "tha-ap411:46.2_0": "tiṇṇaṁ vedāna pāragū;",
  "tha-ap411:46.3_0": "Uttamatthaṁ gavesanto,",
  "tha-ap411:46.4_0": "carissati mahiṁ imaṁ.",
  "tha-ap411:47.1_0": "So pacchā pabbajitvāna,",
  "tha-ap411:47.2_0": "sukkamūlena codito;",
  "tha-ap411:47.3_0": "Gotamassa bhagavato,",
  "tha-ap411:47.4_0": "sāsanebhiramissati.",
  "tha-ap411:48.1_0": "Ārādhayitvāna sambuddhaṁ,",
  "tha-ap411:48.2_0": "gotamaṁ sakyapuṅgavaṁ;",
  "tha-ap411:48.3_0": "Kilese jhāpayitvāna,",
  "tha-ap411:48.4_0": "arahāyaṁ bhavissati’.",
  "tha-ap411:49.1_0": "Vipine byaggharājāva,",
  "tha-ap411:49.2_0": "migarājāva kesarī;",
  "tha-ap411:49.3_0": "Abhīto viharāmajja,",
  "tha-ap411:49.4_0": "sakyaputtassa sāsane.",
  "tha-ap411:50.1_0": "Devaloke manusse vā,",
  "tha-ap411:50.2_0": "dalidde duggatimhi vā;",
  "tha-ap411:50.3_0": "Nibbattiṁ me na passāmi,",
  "tha-ap411:50.4_0": "upaṭṭhānassidaṁ phalaṁ.",
  "tha-ap411:51.1_0": "Vivekamanuyuttomhi,",
  "tha-ap411:51.2_0": "upasanto nirūpadhi;",
  "tha-ap411:51.3_0": "Nāgova bandhanaṁ chetvā,",
  "tha-ap411:51.4_0": "viharāmi anāsavo.",
  "tha-ap411:52.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap411:52.2_0": "…pe…",
  "tha-ap411:52.3_0": "viharāmi anāsavo.",
  "tha-ap411:53.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap411:53.2_0": "…pe…",
  "tha-ap411:53.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap411:54.1_0": "Paṭisambhidā catasso,",
  "tha-ap411:54.2_0": "…pe…",
  "tha-ap411:54.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap411:55.1_0": "Itthaṁ sudaṁ āyasmā jatukaṇṇitthero imā gāthāyo abhāsitthāti.",
  "tha-ap411:56.1_0": "Jatukaṇṇittherassāpadānaṁ navamaṁ."
}