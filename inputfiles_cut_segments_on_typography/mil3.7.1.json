{
  "mil3.7.1:0.1_0": "Milindapañha",
  "mil3.7.1:0.2_0": "Arūpadhammavavatthānavagga",
  "mil3.7.1:0.3_0": "1. Satiuppajjanapañha",
  "mil3.7.1:1.1_0": "Rājā āha—",
  "mil3.7.1:1.2_0": "“bhante nāgasena, katihākārehi sati uppajjatī”ti?",
  "mil3.7.1:1.3_0": "“Sattarasahākārehi, mahārāja, sati uppajjatī”ti.",
  "mil3.7.1:1.4_0": "“Katamehi sattarasahākārehī”ti?",
  "mil3.7.1:1.5_0": "“Abhijānatopi, mahārāja,",
  "mil3.7.1:1.5_1": "sati uppajjati,",
  "mil3.7.1:1.5_2": "kaṭumikāyapi sati uppajjati,",
  "mil3.7.1:1.5_3": "oḷārikaviññāṇatopi sati uppajjati,",
  "mil3.7.1:1.5_4": "hitaviññāṇatopi sati uppajjati,",
  "mil3.7.1:1.5_5": "ahitaviññāṇatopi sati uppajjati,",
  "mil3.7.1:1.5_6": "sabhāganimittatopi sati uppajjati,",
  "mil3.7.1:1.5_7": "visabhāganimittatopi sati uppajjati,",
  "mil3.7.1:1.5_8": "kathābhiññāṇatopi sati uppajjati,",
  "mil3.7.1:1.5_9": "lakkhaṇatopi sati uppajjati,",
  "mil3.7.1:1.5_10": "sāraṇatopi sati uppajjati,",
  "mil3.7.1:1.5_11": "muddātopi sati uppajjati,",
  "mil3.7.1:1.5_12": "gaṇanātopi sati uppajjati,",
  "mil3.7.1:1.5_13": "dhāraṇatopi sati uppajjati,",
  "mil3.7.1:1.5_14": "bhāvanatopi sati uppajjati,",
  "mil3.7.1:1.5_15": "potthakanibandhanatopi sati uppajjati,",
  "mil3.7.1:1.5_16": "upanikkhepatopi sati uppajjati,",
  "mil3.7.1:1.5_17": "anubhūtatopi sati uppajjatīti.",
  "mil3.7.1:2.1_0": "Kathaṁ abhijānato sati uppajjati?",
  "mil3.7.1:2.2_0": "Yathā, mahārāja,",
  "mil3.7.1:2.2_1": "āyasmā ca ānando khujjuttarā ca upāsikā,",
  "mil3.7.1:2.2_2": "ye vā pana aññepi keci jātissarā jātiṁ saranti,",
  "mil3.7.1:2.2_3": "evaṁ abhijānato sati uppajjati.",
  "mil3.7.1:3.1_0": "Kathaṁ kaṭumikāya sati uppajjati?",
  "mil3.7.1:3.2_0": "Yo pakatiyā muṭṭhassatiko, pare ca taṁ sarāpanatthaṁ nibandhanti, evaṁ kaṭumikāya sati uppajjati.",
  "mil3.7.1:4.1_0": "Kathaṁ oḷārikaviññāṇato sati uppajjati?",
  "mil3.7.1:4.2_0": "Yadā rajje vā abhisitto hoti, sotāpattiphalaṁ vā patto hoti, evaṁ oḷārikaviññāṇato sati uppajjati.",
  "mil3.7.1:5.1_0": "Kathaṁ hitaviññāṇato sati uppajjati?",
  "mil3.7.1:5.2_0": "Yamhi sukhāpito, ‘amukasmiṁ evaṁ sukhāpito’ti sarati, evaṁ hitaviññāṇato sati uppajjati.",
  "mil3.7.1:6.1_0": "Kathaṁ ahitaviññāṇato sati uppajjati?",
  "mil3.7.1:6.2_0": "Yamhi dukkhāpito, ‘amukasmiṁ evaṁ dukkhāpito’ti sarati, evaṁ ahitaviññāṇato sati uppajjati.",
  "mil3.7.1:7.1_0": "Kathaṁ sabhāganimittato sati uppajjati?",
  "mil3.7.1:7.2_0": "Sadisaṁ puggalaṁ disvā mātaraṁ vā pitaraṁ vā bhātaraṁ vā bhaginiṁ vā sarati,",
  "mil3.7.1:7.2_1": "oṭṭhaṁ vā goṇaṁ vā gadrabhaṁ vā disvā aññaṁ tādisaṁ oṭṭhaṁ vā goṇaṁ vā gadrabhaṁ vā sarati,",
  "mil3.7.1:7.2_2": "evaṁ sabhāganimittato sati uppajjati.",
  "mil3.7.1:8.1_0": "Kathaṁ visabhāganimittato sati uppajjati?",
  "mil3.7.1:8.2_0": "Asukassa nāma vaṇṇo ediso,",
  "mil3.7.1:8.2_1": "saddo ediso,",
  "mil3.7.1:8.2_2": "gandho ediso,",
  "mil3.7.1:8.2_3": "raso ediso,",
  "mil3.7.1:8.2_4": "phoṭṭhabbo edisoti sarati,",
  "mil3.7.1:8.2_5": "evampi visabhāganimittatopi sati uppajjati.",
  "mil3.7.1:9.1_0": "Kathaṁ kathābhiññāṇato sati uppajjati?",
  "mil3.7.1:9.2_0": "Yo pakatiyā muṭṭhassatiko hoti,",
  "mil3.7.1:9.2_1": "taṁ pare sarāpenti,",
  "mil3.7.1:9.2_2": "tena so sarati,",
  "mil3.7.1:9.2_3": "evaṁ kathābhiññāṇato sati uppajjati.",
  "mil3.7.1:10.1_0": "Kathaṁ lakkhaṇato sati uppajjati?",
  "mil3.7.1:10.2_0": "Yo pakatiyā balībaddānaṁ aṅgena jānāti, lakkhaṇena jānāti, evaṁ lakkhaṇato sati uppajjati.",
  "mil3.7.1:11.1_0": "Kathaṁ sāraṇato sati uppajjati?",
  "mil3.7.1:11.2_0": "Yo pakatiyā muṭṭhassatiko hoti,",
  "mil3.7.1:11.2_1": "yo taṁ ‘sarāhi bho,",
  "mil3.7.1:11.2_2": "sarāhi bho’ti punappunaṁ sarāpeti,",
  "mil3.7.1:11.2_3": "evaṁ sāraṇato sati uppajjati.",
  "mil3.7.1:12.1_0": "Kathaṁ muddāto sati uppajjati?",
  "mil3.7.1:12.2_0": "Lipiyā sikkhitattā jānāti ‘imassa akkharassa anantaraṁ imaṁ akkharaṁ kātabban’ti evaṁ muddāto sati uppajjati.",
  "mil3.7.1:13.1_0": "Kathaṁ gaṇanāto sati uppajjati?",
  "mil3.7.1:13.2_0": "Gaṇanāya sikkhitattā gaṇakā bahumpi gaṇenti, evaṁ gaṇanāto sati uppajjati.",
  "mil3.7.1:14.1_0": "Kathaṁ dhāraṇato sati uppajjati?",
  "mil3.7.1:14.2_0": "Dhāraṇāya sikkhitattā dhāraṇakā bahumpi dhārenti, evaṁ dhāraṇato sati uppajjati.",
  "mil3.7.1:15.1_0": "Kathaṁ bhāvanāto sati uppajjati?",
  "mil3.7.1:15.2_0": "Idha bhikkhu anekavihitaṁ pubbenivāsaṁ anussarati, seyyathidaṁ—",
  "mil3.7.1:15.3_0": "ekampi jātiṁ dvepi jātiyo …pe…",
  "mil3.7.1:15.4_0": "iti sākāraṁ sauddesaṁ pubbenivāsaṁ anussarati, evaṁ bhāvanāto sati uppajjati.",
  "mil3.7.1:16.1_0": "Kathaṁ potthakanibandhanato sati uppajjati?",
  "mil3.7.1:16.2_0": "Rājāno anusāsaniyaṁ assarantā etaṁ potthakaṁ āharathāti,",
  "mil3.7.1:16.2_1": "tena potthakena anussaranti,",
  "mil3.7.1:16.2_2": "evaṁ potthakanibandhanato sati uppajjati.",
  "mil3.7.1:17.1_0": "Kathaṁ upanikkhepato sati uppajjati?",
  "mil3.7.1:17.2_0": "Upanikkhittaṁ bhaṇḍaṁ disvā sarati, evaṁ upanikkhepato sati uppajjati.",
  "mil3.7.1:18.1_0": "Kathaṁ anubhūtato sati uppajjati?",
  "mil3.7.1:18.2_0": "Diṭṭhattā rūpaṁ sarati,",
  "mil3.7.1:18.2_1": "sutattā saddaṁ sarati,",
  "mil3.7.1:18.2_2": "ghāyitattā gandhaṁ sarati,",
  "mil3.7.1:18.2_3": "sāyitattā rasaṁ sarati,",
  "mil3.7.1:18.2_4": "phuṭṭhattā phoṭṭhabbaṁ sarati,",
  "mil3.7.1:18.2_5": "viññātattā dhammaṁ sarati,",
  "mil3.7.1:18.2_6": "evaṁ anubhūtato sati uppajjati.",
  "mil3.7.1:18.3_0": "Imehi kho, mahārāja, sattarasahākārehi sati uppajjatī”ti.",
  "mil3.7.1:19.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.7.1:20.1_0": "Satiuppajjanapañho paṭhamo."
}