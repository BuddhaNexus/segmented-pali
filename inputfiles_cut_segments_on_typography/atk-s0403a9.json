{
  "atk-s0403a9:0_0": "(10) 5. Kakudhavaggo",
  "atk-s0403a9:1_0": "1-2. Sampadāsuttadvayavaṇṇanā",
  "atk-s0403a9:2_0": "91- 92.",
  "atk-s0403a9:2_1": "Pañcamassa paṭhame pañca sampadā missikā kathitā.",
  "atk-s0403a9:2_2": "Dutiye purimā catasso missikā,",
  "atk-s0403a9:2_3": "pañcamī lokikāva.",
  "atk-s0403a9:3_0": "3. Byākaraṇasuttavaṇṇanā",
  "atk-s0403a9:4_0": "93.",
  "atk-s0403a9:4_1": "Tatiye aññābyākaraṇānīti arahattabyākaraṇāni.",
  "atk-s0403a9:4_2": "Mandattāti mandabhāvena aññāṇena.",
  "atk-s0403a9:4_3": "Momūhattāti atimūḷhabhāvena.",
  "atk-s0403a9:4_4": "Aññaṁ byākarotīti arahattaṁ pattosmīti katheti.",
  "atk-s0403a9:4_5": "Icchāpakatoti icchāya abhibhūto.",
  "atk-s0403a9:4_6": "Adhimānenāti adhigatamānena.",
  "atk-s0403a9:4_7": "Sammadevāti hetunā nayena kāraṇeneva.",
  "atk-s0403a9:5_0": "4-5. Phāsuvihārasuttādivaṇṇanā",
  "atk-s0403a9:6_0": "94-95. Catutthe phāsuvihārāti sukhavihārā. Pañcame akuppanti arahattaṁ.",
  "atk-s0403a9:7_0": "6. Sutadharasuttavaṇṇanā",
  "atk-s0403a9:8_0": "96.",
  "atk-s0403a9:8_1": "Chaṭṭhe appaṭṭhoti appasamārambho.",
  "atk-s0403a9:8_2": "Appakiccoti appakaraṇīyo.",
  "atk-s0403a9:8_3": "Subharoti sukhena bharitabbo suposo.",
  "atk-s0403a9:8_4": "Susantosoti tīhi santosehi suṭṭhu santoso.",
  "atk-s0403a9:8_5": "Jīvitaparikkhāresūti jīvitasambhāresu.",
  "atk-s0403a9:8_6": "Appāhāroti mandāhāro.",
  "atk-s0403a9:8_7": "Anodarikattanti na odarikabhāvaṁ amahagghasabhāvaṁ anuyutto.",
  "atk-s0403a9:8_8": "Appamiddhoti na bahuniddo.",
  "atk-s0403a9:8_9": "Sattamaṭṭhamāni uttānatthāni.",
  "atk-s0403a9:9_0": "9. Sīhasuttavaṇṇanā",
  "atk-s0403a9:10_0": "99.",
  "atk-s0403a9:10_1": "Navame sakkaccaññeva deti no asakkaccanti anavaññāya avirajjhitvāva deti,",
  "atk-s0403a9:10_2": "no avaññāya virajjhitvā.",
  "atk-s0403a9:10_3": "Mā me yoggapatho nassāti mayā katayoggapatho mayhaṁ mā nassatu,",
  "atk-s0403a9:10_4": "“eko sīho uṭṭhāya biḷāraṁ paharanto virajjhitvā paharī”ti evaṁ vattāro mā hontūti attho.",
  "atk-s0403a9:10_5": "Annabhāranesādānanti ettha annaṁ vuccati yavabhattaṁ,",
  "atk-s0403a9:10_6": "taṁ bhāro etesanti annabhārā.",
  "atk-s0403a9:10_7": "Yācakānaṁ etaṁ nāmaṁ.",
  "atk-s0403a9:10_8": "Nesādā vuccanti sākuṇikā.",
  "atk-s0403a9:10_9": "Iti sabbapacchimāya koṭiyā etesaṁ yācakanesādānampi sakkaccameva deseti.",
  "atk-s0403a9:11_0": "10. Kakudhatherasuttavaṇṇanā",
  "atk-s0403a9:12_0": "100.",
  "atk-s0403a9:12_1": "Dasame attabhāvapaṭilābhoti sarīrapaṭilābho.",
  "atk-s0403a9:12_2": "Dve vā tīṇi vā māgadhakāni gāmakkhettānīti ettha māgadhikaṁ gāmakkhettaṁ atthi khuddakaṁ,",
  "atk-s0403a9:12_3": "atthi majjhimaṁ,",
  "atk-s0403a9:12_4": "atthi mahantaṁ.",
  "atk-s0403a9:12_5": "Khuddakaṁ gāmakkhettaṁ ito cattālīsaṁ usabhāni,",
  "atk-s0403a9:12_6": "ito cattālīsanti gāvutaṁ hoti,",
  "atk-s0403a9:12_7": "majjhimaṁ ito gāvutaṁ,",
  "atk-s0403a9:12_8": "ito gāvutanti aḍḍhayojanaṁ hoti,",
  "atk-s0403a9:12_9": "mahantaṁ ito diyaḍḍhagāvutaṁ,",
  "atk-s0403a9:12_10": "ito diyaḍḍhagāvutanti tigāvutaṁ hoti.",
  "atk-s0403a9:12_11": "Tesu khuddakena gāmakkhettena tīṇi,",
  "atk-s0403a9:12_12": "khuddakena ca majjhimena ca dve gāmakkhettāni tassa attabhāvo.",
  "atk-s0403a9:12_13": "Tigāvutañhissa sarīraṁ.",
  "atk-s0403a9:12_14": "Pariharissāmīti paṭijaggissāmi gopayissāmi.",
  "atk-s0403a9:12_15": "Rakkhassetanti rakkhassu etaṁ.",
  "atk-s0403a9:12_16": "Moghapurisoti tucchapuriso.",
  "atk-s0403a9:12_17": "Nāssassāti na etassa bhaveyya.",
  "atk-s0403a9:12_18": "Samudācareyyāmāti katheyyāma.",
  "atk-s0403a9:12_19": "Sammannatīti sammānaṁ karoti.",
  "atk-s0403a9:12_20": "Yaṁ tumo karissati tumova tena paññāyissatīti yaṁ esa karissati,",
  "atk-s0403a9:12_21": "esova tena kammena pākaṭo bhavissati.",
  "atk-s0403a9:12_22": "Sesaṁ sabbattha uttānatthamevāti.",
  "atk-s0403a9:13_0": "Kakudhavaggo pañcamo.",
  "atk-s0403a9:14_0": "Dutiyapaṇṇāsakaṁ niṭṭhitaṁ.",
  "atk-s0403a9:15_0": "3. Tatiyapaṇṇāsakaṁ"
}