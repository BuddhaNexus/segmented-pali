{
  "tha-ap482:0.1_0": "Therāpadāna",
  "tha-ap482:0.2_0": "Naḷamālivagga",
  "tha-ap482:0.3_0": "10 Pulinacaṅkamiyattheraapadāna",
  "tha-ap482:1.1_0": "“Migaluddo pure āsiṁ,",
  "tha-ap482:1.2_0": "araññe kānane ahaṁ;",
  "tha-ap482:1.3_0": "Vātamigaṁ gavesanto,",
  "tha-ap482:1.4_0": "caṅkamaṁ addasaṁ ahaṁ.",
  "tha-ap482:2.1_0": "Ucchaṅgena pulinaṁ gayha,",
  "tha-ap482:2.2_0": "caṅkame okiriṁ ahaṁ;",
  "tha-ap482:2.3_0": "Pasannacitto sumano,",
  "tha-ap482:2.4_0": "sugatassa sirīmato.",
  "tha-ap482:3.1_0": "Ekatiṁse ito kappe,",
  "tha-ap482:3.2_0": "pulinaṁ okiriṁ ahaṁ;",
  "tha-ap482:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap482:3.4_0": "pulinassa idaṁ phalaṁ.",
  "tha-ap482:4.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap482:4.2_0": "…pe…",
  "tha-ap482:4.3_0": "viharāmi anāsavo.",
  "tha-ap482:5.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap482:5.2_0": "…pe…",
  "tha-ap482:5.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap482:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap482:6.2_0": "…pe…",
  "tha-ap482:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap482:7.1_0": "Itthaṁ sudaṁ āyasmā pulinacaṅkamiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap482:8.1_0": "Pulinacaṅkamiyattherassāpadānaṁ dasamaṁ.",
  "tha-ap482:9.1_0": "Naḷamālivaggo aṭṭhacattālīsamo.",
  "tha-ap482:10.1_0": "Tassuddānaṁ",
  "tha-ap482:11.1_0": "Naḷamālī maṇidado,",
  "tha-ap482:11.2_0": "ukkāsatikabījanī;",
  "tha-ap482:11.3_0": "Kummāso ca kusaṭṭho ca,",
  "tha-ap482:11.4_0": "giripunnāgiyopi ca.",
  "tha-ap482:12.1_0": "Vallikāro pānadhido,",
  "tha-ap482:12.2_0": "atho pulinacaṅkamo;",
  "tha-ap482:12.3_0": "Gāthāyo pañcanavuti,",
  "tha-ap482:12.4_0": "gaṇitāyo vibhāvibhi."
}