{
  "tika-abh06t0:0_0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "tika-abh06t0:1_0": "Abhidhammāvatāro",
  "tika-abh06t0:2_0": "Ganthārambhakathā",
  "tika-abh06t0:3_0": "1.",
  "tika-abh06t0:4_0": "Anantakaruṇāpaññaṁ, tathāgatamanuttaraṁ;",
  "tika-abh06t0:5_0": "Vanditvā sirasā buddhaṁ, dhammaṁ sādhugaṇampi ca.",
  "tika-abh06t0:6_0": "2.",
  "tika-abh06t0:7_0": "Paṇḍukambalanāmāya, silāyātulavikkamo;",
  "tika-abh06t0:8_0": "Nisinno devarājassa, vimale sītale tale.",
  "tika-abh06t0:9_0": "3.",
  "tika-abh06t0:10_0": "Yaṁ devadevo devānaṁ, devadevehi pūjito;",
  "tika-abh06t0:11_0": "Desesi devalokasmiṁ, dhammaṁ devapurakkhato.",
  "tika-abh06t0:12_0": "4.",
  "tika-abh06t0:13_0": "Tatthāhaṁ pāṭavatthāya, bhikkhūnaṁ piṭakuttame;",
  "tika-abh06t0:14_0": "Abhidhammāvatārantu, madhuraṁ mativaḍḍhanaṁ.",
  "tika-abh06t0:15_0": "5.",
  "tika-abh06t0:16_0": "Tāḷaṁ mohakavāṭassa, vighāṭanamanuttaraṁ;",
  "tika-abh06t0:17_0": "Bhikkhūnaṁ pavisantānaṁ, abhidhammamahāpuraṁ.",
  "tika-abh06t0:18_0": "6.",
  "tika-abh06t0:19_0": "Suduttaraṁ tarantānaṁ, abhidhammamahodadhiṁ;",
  "tika-abh06t0:20_0": "Suduttaraṁ tarantānaṁ, taraṁva makarākaraṁ.",
  "tika-abh06t0:21_0": "7.",
  "tika-abh06t0:22_0": "Ābhidhammikabhikkhūnaṁ, hatthasāramanuttaraṁ;",
  "tika-abh06t0:23_0": "Pavakkhāmi samāsena, taṁ suṇātha samāhitā."
}