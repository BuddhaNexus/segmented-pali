{
  "vv70:0.1_0": "Vimānavatthu",
  "vv70:0.2_0": "Purisavimāna",
  "vv70:0.3_0": "Pāyāsivagga",
  "vv70:0.4_0": "6. Bhikkhādāyakavimānavatthu",
  "vv70:1.1_0": "“Uccamidaṁ maṇithūṇaṁ vimānaṁ,",
  "vv70:1.2_0": "Samantato dvādasa yojanāni;",
  "vv70:1.3_0": "Kūṭāgārā sattasatā uḷārā,",
  "vv70:1.4_0": "Veḷuriyathambhā rucakatthatā subhā.",
  "vv70:2.1_0": "Deviddhipattosi mahānubhāvo,",
  "vv70:2.2_0": "Manussabhūto kimakāsi puññaṁ;",
  "vv70:2.3_0": "Kenāsi evaṁ jalitānubhāvo,",
  "vv70:2.4_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv70:3.1_0": "So devaputto attamano,",
  "vv70:3.2_0": "…pe…",
  "vv70:3.3_0": "yassa kammassidaṁ phalaṁ.",
  "vv70:4.1_0": "“Ahaṁ manussesu manussabhūto,",
  "vv70:4.2_0": "Disvāna bhikkhuṁ tasitaṁ kilantaṁ;",
  "vv70:4.3_0": "Ekāhaṁ bhikkhaṁ paṭipādayissaṁ,",
  "vv70:4.4_0": "Samaṅgi bhattena tadā akāsiṁ.",
  "vv70:5.1_0": "Tena metādiso vaṇṇo,",
  "vv70:5.2_0": "…pe…",
  "vv70:5.3_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv70:6.1_0": "Bhikkhādāyakavimānaṁ chaṭṭhaṁ."
}