{
  "thag1.23:0.1_0": "Theragāthā 1.23",
  "thag1.23:0.2_0": "Ekakanipāta",
  "thag1.23:0.3_0": "Tatiyavagga",
  "thag1.23:0.4_0": "Gosālattheragāthā",
  "thag1.23:1.1_0": "“Ahaṁ kho veḷugumbasmiṁ,",
  "thag1.23:1.2_0": "bhutvāna madhupāyasaṁ;",
  "thag1.23:1.3_0": "Padakkhiṇaṁ sammasanto,",
  "thag1.23:1.4_0": "khandhānaṁ udayabbayaṁ;",
  "thag1.23:1.5_0": "Sānuṁ paṭigamissāmi,",
  "thag1.23:1.6_0": "vivekamanubrūhayan”ti.",
  "thag1.23:1.7_0": "… Gosālo thero …."
}