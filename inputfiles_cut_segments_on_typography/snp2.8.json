{
  "snp2.8:0.1_0": "Sutta Nipāta 2.8",
  "snp2.8:0.2_0": "Dhamma (nāvā) sutta",
  "snp2.8:1.1_0": "Yasmā hi dhammaṁ puriso vijaññā,",
  "snp2.8:1.2_0": "Indaṁva naṁ devatā pūjayeyya;",
  "snp2.8:1.3_0": "So pūjito tasmi pasannacitto,",
  "snp2.8:1.4_0": "Bahussuto pātukaroti dhammaṁ.",
  "snp2.8:2.1_0": "Tadaṭṭhikatvāna nisamma dhīro,",
  "snp2.8:2.2_0": "Dhammānudhammaṁ paṭipajjamāno;",
  "snp2.8:2.3_0": "Viññū vibhāvī nipuṇo ca hoti,",
  "snp2.8:2.4_0": "Yo tādisaṁ bhajati appamatto.",
  "snp2.8:3.1_0": "Khuddañca bālaṁ upasevamāno,",
  "snp2.8:3.2_0": "Anāgatatthañca usūyakañca;",
  "snp2.8:3.3_0": "Idheva dhammaṁ avibhāvayitvā,",
  "snp2.8:3.4_0": "Avitiṇṇakaṅkho maraṇaṁ upeti.",
  "snp2.8:4.1_0": "Yathā naro āpagamotaritvā,",
  "snp2.8:4.2_0": "Mahodakaṁ salilaṁ sīghasotaṁ;",
  "snp2.8:4.3_0": "So vuyhamāno anusotagāmī,",
  "snp2.8:4.4_0": "Kiṁ so pare sakkhati tārayetuṁ.",
  "snp2.8:5.1_0": "Tatheva dhammaṁ avibhāvayitvā,",
  "snp2.8:5.2_0": "Bahussutānaṁ anisāmayatthaṁ;",
  "snp2.8:5.3_0": "Sayaṁ ajānaṁ avitiṇṇakaṅkho,",
  "snp2.8:5.4_0": "Kiṁ so pare sakkhati nijjhapetuṁ.",
  "snp2.8:6.1_0": "Yathāpi nāvaṁ daḷhamāruhitvā,",
  "snp2.8:6.2_0": "Phiyena rittena samaṅgibhūto;",
  "snp2.8:6.3_0": "So tāraye tattha bahūpi aññe,",
  "snp2.8:6.4_0": "Tatrūpayaññū kusalo mutīmā.",
  "snp2.8:7.1_0": "Evampi yo vedagu bhāvitatto,",
  "snp2.8:7.2_0": "Bahussuto hoti avedhadhammo;",
  "snp2.8:7.3_0": "So kho pare nijjhapaye pajānaṁ,",
  "snp2.8:7.4_0": "Sotāvadhānūpanisūpapanne.",
  "snp2.8:8.1_0": "Tasmā have sappurisaṁ bhajetha,",
  "snp2.8:8.2_0": "Medhāvinañceva bahussutañca;",
  "snp2.8:8.3_0": "Aññāya atthaṁ paṭipajjamāno,",
  "snp2.8:8.4_0": "Viññātadhammo sa sukhaṁ labhethāti.",
  "snp2.8:8.5_0": "Nāvāsuttaṁ aṭṭhamaṁ."
}