{
  "ja10:0.1_0": "Jātaka",
  "ja10:0.2_0": "Ekakanipāta",
  "ja10:0.3_0": "Apaṇṇakavagga",
  "ja10:0.4_0": "10. Sukhavihārijātaka",
  "ja10:1.1_0": "“Yañca aññe na rakkhanti,",
  "ja10:1.2_0": "yo ca aññe na rakkhati;",
  "ja10:1.3_0": "Sa ve rāja sukhaṁ seti,",
  "ja10:1.4_0": "kāmesu anapekkhavā”ti.",
  "ja10:2.1_0": "Sukhavihārijātakaṁ dasamaṁ.",
  "ja10:2.2_0": "Apaṇṇakavaggo paṭhamo.",
  "ja10:3.1_0": "Tassuddānaṁ",
  "ja10:4.1_0": "Varāpaṇṇaka vaṇṇupatha serivaro,",
  "ja10:4.2_0": "Suvicakkhaṇa taṇḍulanāḷikassā;",
  "ja10:4.3_0": "Hiri puttavaruttagāmaṇinā,",
  "ja10:4.4_0": "Yo ca na rakkhati tena dasāti."
}