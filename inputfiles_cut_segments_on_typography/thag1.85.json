{
  "thag1.85:0.1_0": "Theragāthā 1.85",
  "thag1.85:0.2_0": "Ekakanipāta",
  "thag1.85:0.3_0": "Navamavagga",
  "thag1.85:0.4_0": "Sunāgattheragāthā",
  "thag1.85:1.1_0": "“Cittanimittassa kovido,",
  "thag1.85:1.2_0": "Pavivekarasaṁ vijāniya;",
  "thag1.85:1.3_0": "Jhāyaṁ nipako patissato,",
  "thag1.85:1.4_0": "Adhigaccheyya sukhaṁ nirāmisan”ti.",
  "thag1.85:1.5_0": "… Sunāgo thero …."
}