{
  "ja63:0.1_0": "Jātaka",
  "ja63:0.2_0": "Ekakanipāta",
  "ja63:0.3_0": "Itthivagga",
  "ja63:0.4_0": "3. Takkapaṇḍitajātaka",
  "ja63:1.1_0": "“Kodhanā akataññū ca,",
  "ja63:1.2_0": "pisuṇā mittabhedikā;",
  "ja63:1.3_0": "Brahmacariyaṁ cara bhikkhu,",
  "ja63:1.4_0": "so sukhaṁ na vihāhasīti.",
  "ja63:2.1_0": "Takkapaṇḍitajātakaṁ tatiyaṁ."
}