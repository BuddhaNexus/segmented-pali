{
  "atk-s0403a11:0_0": "(12) 2. Andhakavindavaggo",
  "atk-s0403a11:1_0": "1. Kulūpakasuttavaṇṇanā",
  "atk-s0403a11:2_0": "111.",
  "atk-s0403a11:2_1": "Dutiyassa paṭhame asanthavavissāsīti attanā saddhiṁ santhavaṁ akarontesu vissāsaṁ anāpajjantesuyeva vissāsaṁ karoti.",
  "atk-s0403a11:2_2": "Anissaravikappīti anissarova samāno “imaṁ detha,",
  "atk-s0403a11:2_3": "imaṁ gaṇhathā”ti issaro viya vikappeti.",
  "atk-s0403a11:2_4": "Vissaṭṭhupasevīti vissaṭṭhāni bhinnakulāni ghaṭanatthāya upasevati.",
  "atk-s0403a11:2_5": "Upakaṇṇakajappīti kaṇṇamūle mantaṁ gaṇhāti.",
  "atk-s0403a11:2_6": "Sukkapakkho vuttavipariyāyena veditabbo.",
  "atk-s0403a11:3_0": "2. Pacchāsamaṇasuttavaṇṇanā",
  "atk-s0403a11:4_0": "112.",
  "atk-s0403a11:4_1": "Dutiye pattapariyāpannaṁ na gaṇhātīti upajjhāye nivattitvā ṭhite attano tucchapattaṁ datvā tassa pattaṁ na gaṇhāti,",
  "atk-s0403a11:4_2": "tato vā dīyamānaṁ na gaṇhāti.",
  "atk-s0403a11:4_3": "Na nivāretīti idaṁ vacanaṁ āpattivītikkamavacanaṁ nāmāti na jānāti.",
  "atk-s0403a11:4_4": "Ñatvā vāpi, “bhante,",
  "atk-s0403a11:4_5": "evarūpaṁ nāma vattuṁ na vaṭṭatī”ti na nivāreti.",
  "atk-s0403a11:4_6": "Kathaṁ opātetīti tassa kathaṁ bhinditvā attano kathaṁ paveseti.",
  "atk-s0403a11:4_7": "Jaḷoti jaḍo.",
  "atk-s0403a11:4_8": "Eḷamūgoti paggharitakheḷamukho.",
  "atk-s0403a11:4_9": "Tatiyaṁ uttānameva.",
  "atk-s0403a11:5_0": "4. Andhakavindasuttavaṇṇanā",
  "atk-s0403a11:6_0": "114.",
  "atk-s0403a11:6_1": "Catutthe sīlavā hothāti sīlavantā hotha.",
  "atk-s0403a11:6_2": "Ārakkhasatinoti dvārarakkhikāya satiyā samannāgatā.",
  "atk-s0403a11:6_3": "Nipakkasatinoti dvārarakkhanakeneva ñāṇena samannāgatassatino.",
  "atk-s0403a11:6_4": "Satārakkhena cetasā samannāgatāti satārakkhena cittena samannāgatā.",
  "atk-s0403a11:6_5": "Appabhassāti appakathā.",
  "atk-s0403a11:6_6": "Sammādiṭṭhikāti kammassakatajjhāna-",
  "atk-s0403a11:6_7": "vipassanāmagga-",
  "atk-s0403a11:6_8": "phalavasena pañcavidhāya sammādiṭṭhiyā samannāgatā.",
  "atk-s0403a11:6_9": "Apica paccavekkhaṇañāṇampi sammādiṭṭhiyevāti veditabbā.",
  "atk-s0403a11:7_0": "5. Maccharinīsuttavaṇṇanā",
  "atk-s0403a11:8_0": "115.",
  "atk-s0403a11:8_1": "Pañcame āvāsamaccharinīti āvāsaṁ maccharāyati,",
  "atk-s0403a11:8_2": "tattha aññesaṁ vāsaṁ na sahati.",
  "atk-s0403a11:8_3": "Kulamaccharinīti upaṭṭhākakulaṁ maccharāyati,",
  "atk-s0403a11:8_4": "aññesaṁ tattha upasaṅkamanaṁ na sahati.",
  "atk-s0403a11:8_5": "Lābhamaccharinīti lābhaṁ maccharāyati,",
  "atk-s0403a11:8_6": "aññesaṁ taṁ uppajjantaṁ na sahati.",
  "atk-s0403a11:8_7": "Vaṇṇamaccharinīti guṇaṁ maccharāyati,",
  "atk-s0403a11:8_8": "aññesaṁ guṇakathaṁ na sahati.",
  "atk-s0403a11:8_9": "Dhammamaccharinīti pariyattidhammaṁ maccharāyati,",
  "atk-s0403a11:8_10": "aññesaṁ dātuṁ na icchati.",
  "atk-s0403a11:9_0": "6-7. Vaṇṇanāsuttādivaṇṇanā",
  "atk-s0403a11:10_0": "116- 117.",
  "atk-s0403a11:10_1": "Chaṭṭhe saddhādeyyaṁ vinipātetīti parehi saddhāya dinnapiṇḍapātato aggaṁ aggahetvā parassa deti.",
  "atk-s0403a11:10_2": "Sattame issukinīti issāya samannāgatā.",
  "atk-s0403a11:10_3": "Sesaṁ sabbattha uttānamevāti.",
  "atk-s0403a11:11_0": "Andhakavindavaggo dutiyo."
}