{
  "tha-ap8:0.1_0": "Therāpadāna",
  "tha-ap8:0.2_0": "Buddhavagga",
  "tha-ap8:0.3_0": "8. Upālittheraapadāna",
  "tha-ap8:1.1_0": "“Nagare haṁsavatiyā,",
  "tha-ap8:1.2_0": "sujāto nāma brāhmaṇo;",
  "tha-ap8:1.3_0": "Asītikoṭinicayo,",
  "tha-ap8:1.4_0": "pahūtadhanadhaññavā.",
  "tha-ap8:2.1_0": "Ajjhāyako mantadharo,",
  "tha-ap8:2.2_0": "tiṇṇaṁ vedāna pāragū;",
  "tha-ap8:2.3_0": "Lakkhaṇe itihāse ca,",
  "tha-ap8:2.4_0": "sadhamme pāramiṁ gato.",
  "tha-ap8:3.1_0": "Paribbājā ekasikhā,",
  "tha-ap8:3.2_0": "gotamā buddhasāvakā;",
  "tha-ap8:3.3_0": "Carakā tāpasā ceva,",
  "tha-ap8:3.4_0": "caranti mahiyā tadā.",
  "tha-ap8:4.1_0": "Tepi maṁ parivārenti,",
  "tha-ap8:4.2_0": "brāhmaṇo vissuto iti;",
  "tha-ap8:4.3_0": "Bahujjano maṁ pūjeti,",
  "tha-ap8:4.4_0": "nāhaṁ pūjemi kiñcanaṁ.",
  "tha-ap8:5.1_0": "Pūjārahaṁ na passāmi,",
  "tha-ap8:5.2_0": "mānatthaddho ahaṁ tadā;",
  "tha-ap8:5.3_0": "Buddhoti vacanaṁ natthi,",
  "tha-ap8:5.4_0": "tāva nuppajjate jino.",
  "tha-ap8:6.1_0": "Accayena ahorattaṁ,",
  "tha-ap8:6.2_0": "padumuttaranāmako;",
  "tha-ap8:6.3_0": "Sabbaṁ tamaṁ vinodetvā,",
  "tha-ap8:6.4_0": "loke uppajji cakkhumā.",
  "tha-ap8:7.1_0": "Vitthārike bāhujaññe,",
  "tha-ap8:7.2_0": "puthubhūte ca sāsane;",
  "tha-ap8:7.3_0": "Upāgami tadā buddho,",
  "tha-ap8:7.4_0": "nagaraṁ haṁsasavhayaṁ.",
  "tha-ap8:8.1_0": "Pitu atthāya so buddho,",
  "tha-ap8:8.2_0": "dhammaṁ desesi cakkhumā;",
  "tha-ap8:8.3_0": "Tena kālena parisā,",
  "tha-ap8:8.4_0": "samantā yojanaṁ tadā.",
  "tha-ap8:9.1_0": "Sammato manujānaṁ so,",
  "tha-ap8:9.2_0": "sunando nāma tāpaso;",
  "tha-ap8:9.3_0": "Yāvatā buddhaparisā,",
  "tha-ap8:9.4_0": "pupphehacchādayī tadā.",
  "tha-ap8:10.1_0": "Catusaccaṁ pakāsente,",
  "tha-ap8:10.2_0": "seṭṭhe ca pupphamaṇḍape;",
  "tha-ap8:10.3_0": "Koṭisatasahassānaṁ,",
  "tha-ap8:10.4_0": "dhammābhisamayo ahu.",
  "tha-ap8:11.1_0": "Sattarattindivaṁ buddho,",
  "tha-ap8:11.2_0": "vassetvā dhammavuṭṭhiyo;",
  "tha-ap8:11.3_0": "Aṭṭhame divase patte,",
  "tha-ap8:11.4_0": "sunandaṁ kittayī jino.",
  "tha-ap8:12.1_0": "Devaloke manusse vā,",
  "tha-ap8:12.2_0": "saṁsaranto ayaṁ bhave;",
  "tha-ap8:12.3_0": "Sabbesaṁ pavaro hutvā,",
  "tha-ap8:12.4_0": "bhavesu saṁsarissati.",
  "tha-ap8:13.1_0": "Kappasatasahassamhi,",
  "tha-ap8:13.2_0": "okkākakulasambhavo;",
  "tha-ap8:13.3_0": "Gotamo nāma gottena,",
  "tha-ap8:13.4_0": "satthā loke bhavissati.",
  "tha-ap8:14.1_0": "Tassa dhammesu dāyādo,",
  "tha-ap8:14.2_0": "oraso dhammanimmito;",
  "tha-ap8:14.3_0": "Mantāṇiputto puṇṇoti,",
  "tha-ap8:14.4_0": "hessati satthu sāvako.",
  "tha-ap8:15.1_0": "Evaṁ kittayi sambuddho,",
  "tha-ap8:15.2_0": "sunandaṁ tāpasaṁ tadā;",
  "tha-ap8:15.3_0": "Hāsayanto janaṁ sabbaṁ,",
  "tha-ap8:15.4_0": "dassayanto sakaṁ balaṁ.",
  "tha-ap8:16.1_0": "Katañjalī namassanti,",
  "tha-ap8:16.2_0": "sunandaṁ tāpasaṁ janā;",
  "tha-ap8:16.3_0": "Buddhe kāraṁ karitvāna,",
  "tha-ap8:16.4_0": "sodhesi gatimattano.",
  "tha-ap8:17.1_0": "Tattha me ahu saṅkappo,",
  "tha-ap8:17.2_0": "sutvāna munino vacaṁ;",
  "tha-ap8:17.3_0": "Ahampi kāraṁ kassāmi,",
  "tha-ap8:17.4_0": "yathā passāmi gotamaṁ.",
  "tha-ap8:18.1_0": "Evāhaṁ cintayitvāna,",
  "tha-ap8:18.2_0": "kiriyaṁ cintayiṁ mama;",
  "tha-ap8:18.3_0": "Kyāhaṁ kammaṁ ācarāmi,",
  "tha-ap8:18.4_0": "puññakkhette anuttare.",
  "tha-ap8:19.1_0": "Ayañca pāṭhiko bhikkhu,",
  "tha-ap8:19.2_0": "sabbapāṭhissa sāsane;",
  "tha-ap8:19.3_0": "Vinaye agganikkhitto,",
  "tha-ap8:19.4_0": "taṁ ṭhānaṁ patthaye ahaṁ.",
  "tha-ap8:20.1_0": "Idaṁ me amitaṁ bhogaṁ,",
  "tha-ap8:20.2_0": "akkhobhaṁ sāgarūpamaṁ;",
  "tha-ap8:20.3_0": "Tena bhogena buddhassa,",
  "tha-ap8:20.4_0": "ārāmaṁ māpaye ahaṁ.",
  "tha-ap8:21.1_0": "Sobhanaṁ nāma ārāmaṁ,",
  "tha-ap8:21.2_0": "nagarassa puratthato;",
  "tha-ap8:21.3_0": "Kiṇitvā satasahassena,",
  "tha-ap8:21.4_0": "saṅghārāmaṁ amāpayiṁ.",
  "tha-ap8:22.1_0": "Kūṭāgāre ca pāsāde,",
  "tha-ap8:22.2_0": "maṇḍape hammiye guhā;",
  "tha-ap8:22.3_0": "Caṅkame sukate katvā,",
  "tha-ap8:22.4_0": "saṅghārāmaṁ amāpayiṁ.",
  "tha-ap8:23.1_0": "Jantāgharaṁ aggisālaṁ,",
  "tha-ap8:23.2_0": "atho udakamāḷakaṁ;",
  "tha-ap8:23.3_0": "Nhānagharaṁ māpayitvā,",
  "tha-ap8:23.4_0": "bhikkhusaṅghassadāsahaṁ.",
  "tha-ap8:24.1_0": "Āsandiyo pīṭhake ca,",
  "tha-ap8:24.2_0": "paribhoge ca bhājane;",
  "tha-ap8:24.3_0": "Ārāmikañca bhesajjaṁ,",
  "tha-ap8:24.4_0": "sabbametaṁ adāsahaṁ.",
  "tha-ap8:25.1_0": "Ārakkhaṁ paṭṭhapetvāna,",
  "tha-ap8:25.2_0": "pākāraṁ kārayiṁ daḷhaṁ;",
  "tha-ap8:25.3_0": "Mā naṁ koci viheṭhesi,",
  "tha-ap8:25.4_0": "santacittāna tādinaṁ.",
  "tha-ap8:26.1_0": "Satasahassenāvāsaṁ,",
  "tha-ap8:26.2_0": "saṅghārāme amāpayiṁ;",
  "tha-ap8:26.3_0": "Vepullaṁ taṁ māpayitvā,",
  "tha-ap8:26.4_0": "sambuddhaṁ upanāmayiṁ.",
  "tha-ap8:27.1_0": "Niṭṭhāpito mayārāmo,",
  "tha-ap8:27.2_0": "sampaṭiccha tuvaṁ muni;",
  "tha-ap8:27.3_0": "Niyyādessāmi taṁ vīra,",
  "tha-ap8:27.4_0": "adhivāsehi cakkhuma.",
  "tha-ap8:28.1_0": "Padumuttaro lokavidū,",
  "tha-ap8:28.2_0": "āhutīnaṁ paṭiggaho;",
  "tha-ap8:28.3_0": "Mama saṅkappamaññāya,",
  "tha-ap8:28.4_0": "adhivāsesi nāyako.",
  "tha-ap8:29.1_0": "Adhivāsanamaññāya,",
  "tha-ap8:29.2_0": "sabbaññussa mahesino;",
  "tha-ap8:29.3_0": "Bhojanaṁ paṭiyādetvā,",
  "tha-ap8:29.4_0": "kālamārocayiṁ ahaṁ.",
  "tha-ap8:30.1_0": "Ārocitamhi kālamhi,",
  "tha-ap8:30.2_0": "padumuttaranāyako;",
  "tha-ap8:30.3_0": "Khīṇāsavasahassehi,",
  "tha-ap8:30.4_0": "ārāmaṁ me upāgami.",
  "tha-ap8:31.1_0": "Nisinnaṁ kālamaññāya,",
  "tha-ap8:31.2_0": "annapānena tappayiṁ;",
  "tha-ap8:31.3_0": "Bhuttāviṁ kālamaññāya,",
  "tha-ap8:31.4_0": "idaṁ vacanamabraviṁ.",
  "tha-ap8:32.1_0": "Kīto satasahassena,",
  "tha-ap8:32.2_0": "tattakeneva kārito;",
  "tha-ap8:32.3_0": "Sobhano nāma ārāmo,",
  "tha-ap8:32.4_0": "sampaṭiccha tuvaṁ muni.",
  "tha-ap8:33.1_0": "Iminārāmadānena,",
  "tha-ap8:33.2_0": "cetanāpaṇidhīhi ca;",
  "tha-ap8:33.3_0": "Bhave nibbattamānohaṁ,",
  "tha-ap8:33.4_0": "labhāmi mama patthitaṁ.",
  "tha-ap8:34.1_0": "Paṭiggahetvā sambuddho,",
  "tha-ap8:34.2_0": "saṅghārāmaṁ sumāpitaṁ;",
  "tha-ap8:34.3_0": "Bhikkhusaṅghe nisīditvā,",
  "tha-ap8:34.4_0": "idaṁ vacanamabravi.",
  "tha-ap8:35.1_0": "Yo so buddhassa pādāsi,",
  "tha-ap8:35.2_0": "saṅghārāmaṁ sumāpitaṁ;",
  "tha-ap8:35.3_0": "Tamahaṁ kittayissāmi,",
  "tha-ap8:35.4_0": "suṇātha mama bhāsato.",
  "tha-ap8:36.1_0": "Hatthī assā rathā pattī,",
  "tha-ap8:36.2_0": "senā ca caturaṅginī;",
  "tha-ap8:36.3_0": "Parivāressantimaṁ niccaṁ,",
  "tha-ap8:36.4_0": "saṅghārāmassidaṁ phalaṁ.",
  "tha-ap8:37.1_0": "Saṭṭhi tūrasahassāni,",
  "tha-ap8:37.2_0": "bheriyo samalaṅkatā;",
  "tha-ap8:37.3_0": "Parivāressantimaṁ niccaṁ,",
  "tha-ap8:37.4_0": "saṅghārāmassidaṁ phalaṁ.",
  "tha-ap8:38.1_0": "Chaḷasītisahassāni,",
  "tha-ap8:38.2_0": "nāriyo samalaṅkatā;",
  "tha-ap8:38.3_0": "Vicittavatthābharaṇā,",
  "tha-ap8:38.4_0": "āmuttamaṇikuṇḍalā.",
  "tha-ap8:39.1_0": "Aḷārapamhā hasulā,",
  "tha-ap8:39.2_0": "susaññā tanumajjhimā;",
  "tha-ap8:39.3_0": "Parivāressantimaṁ niccaṁ,",
  "tha-ap8:39.4_0": "saṅghārāmassidaṁ phalaṁ.",
  "tha-ap8:40.1_0": "Tiṁsakappasahassāni,",
  "tha-ap8:40.2_0": "devaloke ramissati;",
  "tha-ap8:40.3_0": "Sahassakkhattuṁ devindo,",
  "tha-ap8:40.4_0": "devarajjaṁ karissati.",
  "tha-ap8:41.1_0": "Devarājena pattabbaṁ,",
  "tha-ap8:41.2_0": "sabbaṁ paṭilabhissati;",
  "tha-ap8:41.3_0": "Anūnabhogo hutvāna,",
  "tha-ap8:41.4_0": "devarajjaṁ karissati.",
  "tha-ap8:42.1_0": "Sahassakkhattuṁ cakkavattī,",
  "tha-ap8:42.2_0": "rājā raṭṭhe bhavissati;",
  "tha-ap8:42.3_0": "Pathabyā rajjaṁ vipulaṁ,",
  "tha-ap8:42.4_0": "gaṇanāto asaṅkhiyaṁ.",
  "tha-ap8:43.1_0": "Kappasatasahassamhi,",
  "tha-ap8:43.2_0": "okkākakulasambhavo;",
  "tha-ap8:43.3_0": "Gotamo nāma gottena,",
  "tha-ap8:43.4_0": "satthā loke bhavissati.",
  "tha-ap8:44.1_0": "Tassa dhammesu dāyādo,",
  "tha-ap8:44.2_0": "oraso dhammanimmito;",
  "tha-ap8:44.3_0": "Upāli nāma nāmena,",
  "tha-ap8:44.4_0": "hessati satthu sāvako.",
  "tha-ap8:45.1_0": "Vinaye pāramiṁ patvā,",
  "tha-ap8:45.2_0": "ṭhānāṭhāne ca kovido;",
  "tha-ap8:45.3_0": "Jinasāsanaṁ dhārento,",
  "tha-ap8:45.4_0": "viharissatināsavo.",
  "tha-ap8:46.1_0": "Sabbametaṁ abhiññāya,",
  "tha-ap8:46.2_0": "gotamo sakyapuṅgavo;",
  "tha-ap8:46.3_0": "Bhikkhusaṅghe nisīditvā,",
  "tha-ap8:46.4_0": "etadagge ṭhapessati.",
  "tha-ap8:47.1_0": "Aparimeyyupādāya,",
  "tha-ap8:47.2_0": "patthemi tava sāsanaṁ;",
  "tha-ap8:47.3_0": "So me attho anuppatto,",
  "tha-ap8:47.4_0": "sabbasaṁyojanakkhayo.",
  "tha-ap8:48.1_0": "Yathā sūlāvuto poso,",
  "tha-ap8:48.2_0": "rājadaṇḍena tajjito;",
  "tha-ap8:48.3_0": "Sūle sātaṁ avindanto,",
  "tha-ap8:48.4_0": "parimuttiṁva icchati.",
  "tha-ap8:49.1_0": "Tathevāhaṁ mahāvīra,",
  "tha-ap8:49.2_0": "bhavadaṇḍena tajjito;",
  "tha-ap8:49.3_0": "Kammasūlāvuto santo,",
  "tha-ap8:49.4_0": "pipāsāvedanaṭṭito.",
  "tha-ap8:50.1_0": "Bhave sātaṁ na vindāmi,",
  "tha-ap8:50.2_0": "ḍayhanto tīhi aggibhi;",
  "tha-ap8:50.3_0": "Parimuttiṁ gavesāmi,",
  "tha-ap8:50.4_0": "yathāpi rājadaṇḍito.",
  "tha-ap8:51.1_0": "Yathā visādo puriso,",
  "tha-ap8:51.2_0": "visena paripīḷito;",
  "tha-ap8:51.3_0": "Agadaṁ so gaveseyya,",
  "tha-ap8:51.4_0": "visaghātāyupāyanaṁ.",
  "tha-ap8:52.1_0": "Gavesamāno passeyya,",
  "tha-ap8:52.2_0": "agadaṁ visaghātakaṁ;",
  "tha-ap8:52.3_0": "Taṁ pivitvā sukhī assa,",
  "tha-ap8:52.4_0": "visamhā parimuttiyā.",
  "tha-ap8:53.1_0": "Tathevāhaṁ mahāvīra,",
  "tha-ap8:53.2_0": "yathā visahato naro;",
  "tha-ap8:53.3_0": "Sampīḷito avijjāya,",
  "tha-ap8:53.4_0": "saddhammāgadamesahaṁ.",
  "tha-ap8:54.1_0": "Dhammāgadaṁ gavesanto,",
  "tha-ap8:54.2_0": "addakkhiṁ sakyasāsanaṁ;",
  "tha-ap8:54.3_0": "Aggaṁ sabbosadhānaṁ taṁ,",
  "tha-ap8:54.4_0": "sabbasallavinodanaṁ.",
  "tha-ap8:55.1_0": "Dhammosadhaṁ pivitvāna,",
  "tha-ap8:55.2_0": "visaṁ sabbaṁ samūhaniṁ;",
  "tha-ap8:55.3_0": "Ajarāmaraṁ sītibhāvaṁ,",
  "tha-ap8:55.4_0": "nibbānaṁ phassayiṁ ahaṁ.",
  "tha-ap8:56.1_0": "Yathā bhūtaṭṭito poso,",
  "tha-ap8:56.2_0": "bhūtaggāhena pīḷito;",
  "tha-ap8:56.3_0": "Bhūtavejjaṁ gaveseyya,",
  "tha-ap8:56.4_0": "bhūtasmā parimuttiyā.",
  "tha-ap8:57.1_0": "Gavesamāno passeyya,",
  "tha-ap8:57.2_0": "bhūtavijjāsu kovidaṁ;",
  "tha-ap8:57.3_0": "Tassa so vihane bhūtaṁ,",
  "tha-ap8:57.4_0": "samūlañca vināsaye.",
  "tha-ap8:58.1_0": "Tathevāhaṁ mahāvīra,",
  "tha-ap8:58.2_0": "tamaggāhena pīḷito;",
  "tha-ap8:58.3_0": "Ñāṇālokaṁ gavesāmi,",
  "tha-ap8:58.4_0": "tamato parimuttiyā.",
  "tha-ap8:59.1_0": "Athaddasaṁ sakyamuniṁ,",
  "tha-ap8:59.2_0": "kilesatamasodhanaṁ;",
  "tha-ap8:59.3_0": "So me tamaṁ vinodesi,",
  "tha-ap8:59.4_0": "bhūtavejjova bhūtakaṁ.",
  "tha-ap8:60.1_0": "Saṁsārasotaṁ sañchindiṁ,",
  "tha-ap8:60.2_0": "taṇhāsotaṁ nivārayiṁ;",
  "tha-ap8:60.3_0": "Bhavaṁ ugghāṭayiṁ sabbaṁ,",
  "tha-ap8:60.4_0": "bhūtavejjova mūlato.",
  "tha-ap8:61.1_0": "Garuḷo yathā opatati,",
  "tha-ap8:61.2_0": "pannagaṁ bhakkhamattano;",
  "tha-ap8:61.3_0": "Samantā yojanasataṁ,",
  "tha-ap8:61.4_0": "vikkhobheti mahāsaraṁ.",
  "tha-ap8:62.1_0": "Pannagaṁ so gahetvāna,",
  "tha-ap8:62.2_0": "adhosīsaṁ viheṭhayaṁ;",
  "tha-ap8:62.3_0": "Ādāya so pakkamati,",
  "tha-ap8:62.4_0": "yenakāmaṁ vihaṅgamo.",
  "tha-ap8:63.1_0": "Tathevāhaṁ mahāvīra,",
  "tha-ap8:63.2_0": "yathāpi garuḷo balī;",
  "tha-ap8:63.3_0": "Asaṅkhataṁ gavesanto,",
  "tha-ap8:63.4_0": "dose vikkhālayiṁ ahaṁ.",
  "tha-ap8:64.1_0": "Diṭṭho ahaṁ dhammavaraṁ,",
  "tha-ap8:64.2_0": "santipadamanuttaraṁ;",
  "tha-ap8:64.3_0": "Ādāya viharāmetaṁ,",
  "tha-ap8:64.4_0": "garuḷo pannagaṁ yathā.",
  "tha-ap8:65.1_0": "Āsāvatī nāma latā,",
  "tha-ap8:65.2_0": "jātā cittalatāvane;",
  "tha-ap8:65.3_0": "Tassā vassasahassena,",
  "tha-ap8:65.4_0": "ekaṁ nibbattate phalaṁ.",
  "tha-ap8:66.1_0": "Taṁ devā payirupāsanti,",
  "tha-ap8:66.2_0": "tāvadūraphale sati;",
  "tha-ap8:66.3_0": "Devānaṁ sā piyā evaṁ,",
  "tha-ap8:66.4_0": "āsāvatī latuttamā.",
  "tha-ap8:67.1_0": "Satasahassupādāya,",
  "tha-ap8:67.2_0": "tāhaṁ paricare muni;",
  "tha-ap8:67.3_0": "Sāyaṁ pātaṁ namassāmi,",
  "tha-ap8:67.4_0": "devā āsāvatiṁ yathā.",
  "tha-ap8:68.1_0": "Avañjhā pāricariyā,",
  "tha-ap8:68.2_0": "amoghā ca namassanā;",
  "tha-ap8:68.3_0": "Dūrāgatampi maṁ santaṁ,",
  "tha-ap8:68.4_0": "khaṇoyaṁ na virādhayi.",
  "tha-ap8:69.1_0": "Paṭisandhiṁ na passāmi,",
  "tha-ap8:69.2_0": "vicinanto bhave ahaṁ;",
  "tha-ap8:69.3_0": "Nirūpadhi vippamutto,",
  "tha-ap8:69.4_0": "upasanto carāmahaṁ.",
  "tha-ap8:70.1_0": "Yathāpi padumaṁ nāma,",
  "tha-ap8:70.2_0": "sūriyaraṁsena pupphati;",
  "tha-ap8:70.3_0": "Tathevāhaṁ mahāvīra,",
  "tha-ap8:70.4_0": "buddharaṁsena pupphito.",
  "tha-ap8:71.1_0": "Yathā balākayonimhi,",
  "tha-ap8:71.2_0": "na vijjati pumo sadā;",
  "tha-ap8:71.3_0": "Meghesu gajjamānesu,",
  "tha-ap8:71.4_0": "gabbhaṁ gaṇhanti tā sadā.",
  "tha-ap8:72.1_0": "Cirampi gabbhaṁ dhārenti,",
  "tha-ap8:72.2_0": "yāva megho na gajjati;",
  "tha-ap8:72.3_0": "Bhārato parimuccanti,",
  "tha-ap8:72.4_0": "yadā megho pavassati.",
  "tha-ap8:73.1_0": "Padumuttarabuddhassa,",
  "tha-ap8:73.2_0": "dhammameghena gajjato;",
  "tha-ap8:73.3_0": "Saddena dhammameghassa,",
  "tha-ap8:73.4_0": "dhammagabbhaṁ agaṇhahaṁ.",
  "tha-ap8:74.1_0": "Satasahassupādāya,",
  "tha-ap8:74.2_0": "puññagabbhaṁ dharemahaṁ;",
  "tha-ap8:74.3_0": "Nappamuccāmi bhārato,",
  "tha-ap8:74.4_0": "dhammamegho na gajjati.",
  "tha-ap8:75.1_0": "Yadā tuvaṁ sakyamuni,",
  "tha-ap8:75.2_0": "ramme kapilavatthave;",
  "tha-ap8:75.3_0": "Gajjasi dhammameghena,",
  "tha-ap8:75.4_0": "bhārato parimuccahaṁ.",
  "tha-ap8:76.1_0": "Suññataṁ animittañca,",
  "tha-ap8:76.2_0": "Tathāppaṇihitampi ca;",
  "tha-ap8:76.3_0": "Caturo ca phale sabbe,",
  "tha-ap8:76.4_0": "Dhammevaṁ vijanayiṁ ahaṁ.",
  "tha-ap8:77.1_0": "Dutiyabhāṇavāraṁ.",
  "tha-ap8:78.1_0": "Aparimeyyupādāya,",
  "tha-ap8:78.2_0": "patthemi tava sāsanaṁ;",
  "tha-ap8:78.3_0": "So me attho anuppatto,",
  "tha-ap8:78.4_0": "santipadamanuttaraṁ.",
  "tha-ap8:79.1_0": "Vinaye pāramiṁ patto,",
  "tha-ap8:79.2_0": "yathāpi pāṭhiko isi;",
  "tha-ap8:79.3_0": "Na me samasamo atthi,",
  "tha-ap8:79.4_0": "dhāremi sāsanaṁ ahaṁ.",
  "tha-ap8:80.1_0": "Vinaye khandhake cāpi,",
  "tha-ap8:80.2_0": "tikacchede ca pañcame;",
  "tha-ap8:80.3_0": "Ettha me vimati natthi,",
  "tha-ap8:80.4_0": "akkhare byañjanepi vā.",
  "tha-ap8:81.1_0": "Niggahe paṭikamme ca,",
  "tha-ap8:81.2_0": "ṭhānāṭhāne ca kovido;",
  "tha-ap8:81.3_0": "Osāraṇe vuṭṭhāpane,",
  "tha-ap8:81.4_0": "sabbattha pāramiṁ gato.",
  "tha-ap8:82.1_0": "Vinaye khandhake vāpi,",
  "tha-ap8:82.2_0": "nikkhipitvā padaṁ ahaṁ;",
  "tha-ap8:82.3_0": "Ubhato viniveṭhetvā,",
  "tha-ap8:82.4_0": "rasato osareyyahaṁ.",
  "tha-ap8:83.1_0": "Niruttiyā sukusalo,",
  "tha-ap8:83.2_0": "atthānatthe ca kovido;",
  "tha-ap8:83.3_0": "Anaññātaṁ mayā natthi,",
  "tha-ap8:83.4_0": "ekaggo satthu sāsane.",
  "tha-ap8:84.1_0": "Rūpadakkho ahaṁ ajja,",
  "tha-ap8:84.2_0": "sakyaputtassa sāsane;",
  "tha-ap8:84.3_0": "Kaṅkhaṁ sabbaṁ vinodemi,",
  "tha-ap8:84.4_0": "chindāmi sabbasaṁsayaṁ.",
  "tha-ap8:85.1_0": "Padaṁ anupadañcāpi,",
  "tha-ap8:85.2_0": "akkharañcāpi byañjanaṁ;",
  "tha-ap8:85.3_0": "Nidāne pariyosāne,",
  "tha-ap8:85.4_0": "sabbattha kovido ahaṁ.",
  "tha-ap8:86.1_0": "Yathāpi rājā balavā,",
  "tha-ap8:86.2_0": "niggaṇhitvā parantape;",
  "tha-ap8:86.3_0": "Vijinitvāna saṅgāmaṁ,",
  "tha-ap8:86.4_0": "nagaraṁ tattha māpaye.",
  "tha-ap8:87.1_0": "Pākāraṁ parikhañcāpi,",
  "tha-ap8:87.2_0": "esikaṁ dvārakoṭṭhakaṁ;",
  "tha-ap8:87.3_0": "Aṭṭālake ca vividhe,",
  "tha-ap8:87.4_0": "kāraye nagare bahū.",
  "tha-ap8:88.1_0": "Siṅghāṭakaṁ caccarañca,",
  "tha-ap8:88.2_0": "suvibhattantarāpaṇaṁ;",
  "tha-ap8:88.3_0": "Kārayeyya sabhaṁ tattha,",
  "tha-ap8:88.4_0": "atthānatthavinicchayaṁ.",
  "tha-ap8:89.1_0": "Nigghātatthaṁ amittānaṁ,",
  "tha-ap8:89.2_0": "chiddāchiddañca jānituṁ;",
  "tha-ap8:89.3_0": "Balakāyassa rakkhāya,",
  "tha-ap8:89.4_0": "senāpaccaṁ ṭhapeti so.",
  "tha-ap8:90.1_0": "Ārakkhatthāya bhaṇḍassa,",
  "tha-ap8:90.2_0": "nidhānakusalaṁ naraṁ;",
  "tha-ap8:90.3_0": "Mā me bhaṇḍaṁ vinassīti,",
  "tha-ap8:90.4_0": "bhaṇḍarakkhaṁ ṭhapeti so.",
  "tha-ap8:91.1_0": "Mamatto hoti yo rañño,",
  "tha-ap8:91.2_0": "vuddhiṁ yassa ca icchati;",
  "tha-ap8:91.3_0": "Tassādhikaraṇaṁ deti,",
  "tha-ap8:91.4_0": "mittassa paṭipajjituṁ.",
  "tha-ap8:92.1_0": "Uppātesu nimittesu,",
  "tha-ap8:92.2_0": "lakkhaṇesu ca kovidaṁ;",
  "tha-ap8:92.3_0": "Ajjhāyakaṁ mantadharaṁ,",
  "tha-ap8:92.4_0": "porohicce ṭhapeti so.",
  "tha-ap8:93.1_0": "Etehaṅgehi sampanno,",
  "tha-ap8:93.2_0": "khattiyoti pavuccati;",
  "tha-ap8:93.3_0": "Sadā rakkhanti rājānaṁ,",
  "tha-ap8:93.4_0": "cakkavākova dukkhitaṁ.",
  "tha-ap8:94.1_0": "Tatheva tvaṁ mahāvīra,",
  "tha-ap8:94.2_0": "hatāmittova khattiyo;",
  "tha-ap8:94.3_0": "Sadevakassa lokassa,",
  "tha-ap8:94.4_0": "dhammarājāti vuccati.",
  "tha-ap8:95.1_0": "Titthiye nihanitvāna,",
  "tha-ap8:95.2_0": "mārañcāpi sasenakaṁ;",
  "tha-ap8:95.3_0": "Tamandhakāraṁ vidhamitvā,",
  "tha-ap8:95.4_0": "dhammanagaraṁ amāpayi.",
  "tha-ap8:96.1_0": "Sīlaṁ pākārakaṁ tattha,",
  "tha-ap8:96.2_0": "ñāṇaṁ te dvārakoṭṭhakaṁ;",
  "tha-ap8:96.3_0": "Saddhā te esikā vīra,",
  "tha-ap8:96.4_0": "dvārapālo ca saṁvaro.",
  "tha-ap8:97.1_0": "Satipaṭṭhānamaṭṭālaṁ,",
  "tha-ap8:97.2_0": "paññā te caccaraṁ mune;",
  "tha-ap8:97.3_0": "Iddhipādañca siṅghāṭaṁ,",
  "tha-ap8:97.4_0": "dhammavīthi sumāpitā.",
  "tha-ap8:98.1_0": "Suttantaṁ abhidhammañca,",
  "tha-ap8:98.2_0": "vinayañcāpi kevalaṁ;",
  "tha-ap8:98.3_0": "Navaṅgaṁ buddhavacanaṁ,",
  "tha-ap8:98.4_0": "esā dhammasabhā tava.",
  "tha-ap8:99.1_0": "Suññataṁ animittañca,",
  "tha-ap8:99.2_0": "vihārañcappaṇīhitaṁ;",
  "tha-ap8:99.3_0": "Āneñjañca nirodho ca,",
  "tha-ap8:99.4_0": "esā dhammakuṭī tava.",
  "tha-ap8:100.1_0": "Paññāya aggo nikkhitto,",
  "tha-ap8:100.2_0": "paṭibhāne ca kovido;",
  "tha-ap8:100.3_0": "Sāriputtoti nāmena,",
  "tha-ap8:100.4_0": "dhammasenāpatī tava.",
  "tha-ap8:101.1_0": "Cutūpapātakusalo,",
  "tha-ap8:101.2_0": "iddhiyā pāramiṁ gato;",
  "tha-ap8:101.3_0": "Kolito nāma nāmena,",
  "tha-ap8:101.4_0": "porohicco tavaṁ mune.",
  "tha-ap8:102.1_0": "Porāṇakavaṁsadharo,",
  "tha-ap8:102.2_0": "uggatejo durāsado;",
  "tha-ap8:102.3_0": "Dhutavādīguṇenaggo,",
  "tha-ap8:102.4_0": "akkhadasso tavaṁ mune.",
  "tha-ap8:103.1_0": "Bahussuto dhammadharo,",
  "tha-ap8:103.2_0": "Sabbapāṭhī ca sāsane;",
  "tha-ap8:103.3_0": "Ānando nāma nāmena,",
  "tha-ap8:103.4_0": "Dhammārakkho tavaṁ mune.",
  "tha-ap8:104.1_0": "Ete sabbe atikkamma,",
  "tha-ap8:104.2_0": "pamesi bhagavā mamaṁ;",
  "tha-ap8:104.3_0": "Vinicchayaṁ me pādāsi,",
  "tha-ap8:104.4_0": "vinaye viññudesitaṁ.",
  "tha-ap8:105.1_0": "Yo koci vinaye pañhaṁ,",
  "tha-ap8:105.2_0": "pucchati buddhasāvako;",
  "tha-ap8:105.3_0": "Tattha me cintanā natthi,",
  "tha-ap8:105.4_0": "taññevatthaṁ kathemahaṁ.",
  "tha-ap8:106.1_0": "Yāvatā buddhakhettamhi,",
  "tha-ap8:106.2_0": "ṭhapetvā taṁ mahāmuni;",
  "tha-ap8:106.3_0": "Vinaye mādiso natthi,",
  "tha-ap8:106.4_0": "kuto bhiyyo bhavissati.",
  "tha-ap8:107.1_0": "Bhikkhusaṅghe nisīditvā,",
  "tha-ap8:107.2_0": "evaṁ gajjati gotamo;",
  "tha-ap8:107.3_0": "Upālissa samo natthi,",
  "tha-ap8:107.4_0": "vinaye khandhakesu ca.",
  "tha-ap8:108.1_0": "Yāvatā buddhabhaṇitaṁ,",
  "tha-ap8:108.2_0": "navaṅgaṁ satthusāsanaṁ;",
  "tha-ap8:108.3_0": "Vinayogadhaṁ taṁ sabbaṁ,",
  "tha-ap8:108.4_0": "vinayamūlapassino.",
  "tha-ap8:109.1_0": "Mama kammaṁ saritvāna,",
  "tha-ap8:109.2_0": "gotamo sakyapuṅgavo;",
  "tha-ap8:109.3_0": "Bhikkhusaṅghe nisīditvā,",
  "tha-ap8:109.4_0": "etadagge ṭhapesi maṁ.",
  "tha-ap8:110.1_0": "Satasahassupādāya,",
  "tha-ap8:110.2_0": "imaṁ ṭhānaṁ apatthayiṁ;",
  "tha-ap8:110.3_0": "So me attho anuppatto,",
  "tha-ap8:110.4_0": "vinaye pāramiṁ gato.",
  "tha-ap8:111.1_0": "Sakyānaṁ nandijanano,",
  "tha-ap8:111.2_0": "kappako āsahaṁ pure;",
  "tha-ap8:111.3_0": "Vijahitvāna taṁ jātiṁ,",
  "tha-ap8:111.4_0": "putto jāto mahesino.",
  "tha-ap8:112.1_0": "Ito dutiyake kappe,",
  "tha-ap8:112.2_0": "Añjaso nāma khattiyo;",
  "tha-ap8:112.3_0": "Anantatejo amitayaso,",
  "tha-ap8:112.4_0": "Bhūmipālo mahaddhano.",
  "tha-ap8:113.1_0": "Tassa rañño ahaṁ putto,",
  "tha-ap8:113.2_0": "candano nāma khattiyo;",
  "tha-ap8:113.3_0": "Jātimadenupatthaddho,",
  "tha-ap8:113.4_0": "yasabhogamadena ca.",
  "tha-ap8:114.1_0": "Nāgasatasahassāni,",
  "tha-ap8:114.2_0": "sabbālaṅkārabhūsitā;",
  "tha-ap8:114.3_0": "Tidhāpabhinnā mātaṅgā,",
  "tha-ap8:114.4_0": "parivārenti maṁ sadā.",
  "tha-ap8:115.1_0": "Sabalehi paretohaṁ,",
  "tha-ap8:115.2_0": "uyyānaṁ gantukāmako;",
  "tha-ap8:115.3_0": "Āruyha sirikaṁ nāgaṁ,",
  "tha-ap8:115.4_0": "nagarā nikkhamiṁ tadā.",
  "tha-ap8:116.1_0": "Caraṇena ca sampanno,",
  "tha-ap8:116.2_0": "guttadvāro susaṁvuto;",
  "tha-ap8:116.3_0": "Devalo nāma sambuddho,",
  "tha-ap8:116.4_0": "āgacchi purato mama.",
  "tha-ap8:117.1_0": "Pesetvā sirikaṁ nāgaṁ,",
  "tha-ap8:117.2_0": "buddhaṁ āsādayiṁ tadā;",
  "tha-ap8:117.3_0": "Tato sañjātakopo so,",
  "tha-ap8:117.4_0": "nāgo nuddharate padaṁ.",
  "tha-ap8:118.1_0": "Nāgaṁ duṭṭhamanaṁ disvā,",
  "tha-ap8:118.2_0": "buddhe kodhaṁ akāsahaṁ;",
  "tha-ap8:118.3_0": "Vihesayitvā sambuddhaṁ,",
  "tha-ap8:118.4_0": "uyyānaṁ agamāsahaṁ.",
  "tha-ap8:119.1_0": "Sātaṁ tattha na vindāmi,",
  "tha-ap8:119.2_0": "siro pajjalito yathā;",
  "tha-ap8:119.3_0": "Pariḷāhena ḍayhāmi,",
  "tha-ap8:119.4_0": "macchova baḷisādako.",
  "tha-ap8:120.1_0": "Sasāgarantā pathavī,",
  "tha-ap8:120.2_0": "ādittā viya hoti me;",
  "tha-ap8:120.3_0": "Pitu santikupāgamma,",
  "tha-ap8:120.4_0": "idaṁ vacanamabraviṁ.",
  "tha-ap8:121.1_0": "Āsīvisaṁva kupitaṁ,",
  "tha-ap8:121.2_0": "aggikkhandhaṁva āgataṁ;",
  "tha-ap8:121.3_0": "Mattaṁva kuñjaraṁ dantiṁ,",
  "tha-ap8:121.4_0": "yaṁ sayambhumasādayiṁ.",
  "tha-ap8:122.1_0": "Āsādito mayā buddho,",
  "tha-ap8:122.2_0": "ghoro uggatapo jino;",
  "tha-ap8:122.3_0": "Purā sabbe vinassāma,",
  "tha-ap8:122.4_0": "khamāpessāma taṁ muniṁ.",
  "tha-ap8:123.1_0": "No ce taṁ nijjhāpessāma,",
  "tha-ap8:123.2_0": "attadantaṁ samāhitaṁ;",
  "tha-ap8:123.3_0": "Orena sattadivasā,",
  "tha-ap8:123.4_0": "raṭṭhaṁ me vidhamissati.",
  "tha-ap8:124.1_0": "Sumekhalo kosiyo ca,",
  "tha-ap8:124.2_0": "siggavo cāpi sattako;",
  "tha-ap8:124.3_0": "Āsādayitvā isayo,",
  "tha-ap8:124.4_0": "duggatā te saraṭṭhakā.",
  "tha-ap8:125.1_0": "Yadā kuppanti isayo,",
  "tha-ap8:125.2_0": "saññatā brahmacārino;",
  "tha-ap8:125.3_0": "Sadevakaṁ vināsenti,",
  "tha-ap8:125.4_0": "sasāgaraṁ sapabbataṁ.",
  "tha-ap8:126.1_0": "Tiyojanasahassamhi,",
  "tha-ap8:126.2_0": "purise sannipātayiṁ;",
  "tha-ap8:126.3_0": "Accayaṁ desanatthāya,",
  "tha-ap8:126.4_0": "sayambhuṁ upasaṅkamiṁ.",
  "tha-ap8:127.1_0": "Allavatthā allasirā,",
  "tha-ap8:127.2_0": "sabbeva pañjalīkatā;",
  "tha-ap8:127.3_0": "Buddhassa pāde nipatitvā,",
  "tha-ap8:127.4_0": "idaṁ vacanamabravuṁ.",
  "tha-ap8:128.1_0": "Khamassu tvaṁ mahāvīra,",
  "tha-ap8:128.2_0": "abhiyācati taṁ jano;",
  "tha-ap8:128.3_0": "Pariḷāhaṁ vinodehi,",
  "tha-ap8:128.4_0": "mā no raṭṭhaṁ vināsaya.",
  "tha-ap8:129.1_0": "Sadevamānusā sabbe,",
  "tha-ap8:129.2_0": "sadānavā sarakkhasā;",
  "tha-ap8:129.3_0": "Ayomayena kūṭena,",
  "tha-ap8:129.4_0": "siraṁ bhindeyyu me sadā.",
  "tha-ap8:130.1_0": "Dake aggi na saṇṭhāti,",
  "tha-ap8:130.2_0": "bījaṁ sele na rūhati;",
  "tha-ap8:130.3_0": "Agade kimi na saṇṭhāti,",
  "tha-ap8:130.4_0": "kopo buddhe na jāyati.",
  "tha-ap8:131.1_0": "Yathā ca bhūmi acalā,",
  "tha-ap8:131.2_0": "appameyyo ca sāgaro;",
  "tha-ap8:131.3_0": "Anantako ca ākāso,",
  "tha-ap8:131.4_0": "evaṁ buddhā akhobhiyā.",
  "tha-ap8:132.1_0": "Sadā khantā mahāvīrā,",
  "tha-ap8:132.2_0": "khamitā ca tapassino;",
  "tha-ap8:132.3_0": "Khantānaṁ khamitānañca,",
  "tha-ap8:132.4_0": "gamanaṁ taṁ na vijjati.",
  "tha-ap8:133.1_0": "Idaṁ vatvāna sambuddho,",
  "tha-ap8:133.2_0": "pariḷāhaṁ vinodayaṁ;",
  "tha-ap8:133.3_0": "Mahājanassa purato,",
  "tha-ap8:133.4_0": "nabhaṁ abbhuggami tadā.",
  "tha-ap8:134.1_0": "Tena kammenahaṁ vīra,",
  "tha-ap8:134.2_0": "hīnattaṁ ajjhupāgato;",
  "tha-ap8:134.3_0": "Samatikkamma taṁ jātiṁ,",
  "tha-ap8:134.4_0": "pāvisiṁ abhayaṁ puraṁ.",
  "tha-ap8:135.1_0": "Tadāpi maṁ mahāvīra,",
  "tha-ap8:135.2_0": "ḍayhamānaṁ susaṇṭhitaṁ;",
  "tha-ap8:135.3_0": "Pariḷāhaṁ vinodesi,",
  "tha-ap8:135.4_0": "sayambhuñca khamāpayiṁ.",
  "tha-ap8:136.1_0": "Ajjāpi maṁ mahāvīra,",
  "tha-ap8:136.2_0": "ḍayhamānaṁ tihaggibhi;",
  "tha-ap8:136.3_0": "Nibbāpesi tayo aggī,",
  "tha-ap8:136.4_0": "sītibhāvañca pāpayī.",
  "tha-ap8:137.1_0": "Yesaṁ sotāvadhānatthi,",
  "tha-ap8:137.2_0": "suṇātha mama bhāsato;",
  "tha-ap8:137.3_0": "Atthaṁ tumhaṁ pavakkhāmi,",
  "tha-ap8:137.4_0": "yathā diṭṭhaṁ padaṁ mama.",
  "tha-ap8:138.1_0": "Sayambhuṁ taṁ vimānetvā,",
  "tha-ap8:138.2_0": "santacittaṁ samāhitaṁ;",
  "tha-ap8:138.3_0": "Tena kammenahaṁ ajja,",
  "tha-ap8:138.4_0": "jātomhi nīcayoniyaṁ.",
  "tha-ap8:139.1_0": "Mā vo khaṇaṁ virādhetha,",
  "tha-ap8:139.2_0": "khaṇātītā hi socare;",
  "tha-ap8:139.3_0": "Sadatthe vāyameyyātha,",
  "tha-ap8:139.4_0": "khaṇo vo paṭipādito.",
  "tha-ap8:140.1_0": "Ekaccānañca vamanaṁ,",
  "tha-ap8:140.2_0": "ekaccānaṁ virecanaṁ;",
  "tha-ap8:140.3_0": "Visaṁ halāhalaṁ eke,",
  "tha-ap8:140.4_0": "ekaccānañca osadhaṁ.",
  "tha-ap8:141.1_0": "Vamanaṁ paṭipannānaṁ,",
  "tha-ap8:141.2_0": "phalaṭṭhānaṁ virecanaṁ;",
  "tha-ap8:141.3_0": "Osadhaṁ phalalābhīnaṁ,",
  "tha-ap8:141.4_0": "puññakkhettaṁ gavesinaṁ.",
  "tha-ap8:142.1_0": "Sāsanena viruddhānaṁ,",
  "tha-ap8:142.2_0": "visaṁ halāhalaṁ yathā;",
  "tha-ap8:142.3_0": "Āsīviso diṭṭhaviso,",
  "tha-ap8:142.4_0": "evaṁ jhāpeti taṁ naraṁ.",
  "tha-ap8:143.1_0": "Sakiṁ pītaṁ halāhalaṁ,",
  "tha-ap8:143.2_0": "uparundhati jīvitaṁ;",
  "tha-ap8:143.3_0": "Sāsanena virujjhitvā,",
  "tha-ap8:143.4_0": "kappakoṭimhi ḍayhati.",
  "tha-ap8:144.1_0": "Khantiyā avihiṁsāya,",
  "tha-ap8:144.2_0": "mettacittavatāya ca;",
  "tha-ap8:144.3_0": "Sadevakaṁ so tāreti,",
  "tha-ap8:144.4_0": "tasmā vo avirodhiyā.",
  "tha-ap8:145.1_0": "Lābhālābhe na sajjanti,",
  "tha-ap8:145.2_0": "sammānanavimānane;",
  "tha-ap8:145.3_0": "Pathavīsadisā buddhā,",
  "tha-ap8:145.4_0": "tasmā te na virodhiyā.",
  "tha-ap8:146.1_0": "Devadatte ca vadhake,",
  "tha-ap8:146.2_0": "core aṅgulimālake;",
  "tha-ap8:146.3_0": "Rāhule dhanapāle ca,",
  "tha-ap8:146.4_0": "sabbesaṁ samako muni.",
  "tha-ap8:147.1_0": "Etesaṁ paṭigho natthi,",
  "tha-ap8:147.2_0": "rāgomesaṁ na vijjati;",
  "tha-ap8:147.3_0": "Sabbesaṁ samako buddho,",
  "tha-ap8:147.4_0": "vadhakassorasassa ca.",
  "tha-ap8:148.1_0": "Panthe disvāna kāsāvaṁ,",
  "tha-ap8:148.2_0": "chaḍḍitaṁ mīḷhamakkhitaṁ;",
  "tha-ap8:148.3_0": "Sirasmiṁ añjaliṁ katvā,",
  "tha-ap8:148.4_0": "vanditabbaṁ isiddhajaṁ.",
  "tha-ap8:149.1_0": "Abbhatītā ca ye buddhā,",
  "tha-ap8:149.2_0": "vattamānā anāgatā;",
  "tha-ap8:149.3_0": "Dhajenānena sujjhanti,",
  "tha-ap8:149.4_0": "tasmā ete namassiyā.",
  "tha-ap8:150.1_0": "Satthukappaṁ suvinayaṁ,",
  "tha-ap8:150.2_0": "dhāremi hadayenahaṁ;",
  "tha-ap8:150.3_0": "Namassamāno vinayaṁ,",
  "tha-ap8:150.4_0": "viharissāmi sabbadā.",
  "tha-ap8:151.1_0": "Vinayo āsayo mayhaṁ,",
  "tha-ap8:151.2_0": "vinayo ṭhānacaṅkamaṁ;",
  "tha-ap8:151.3_0": "Kappemi vinaye vāsaṁ,",
  "tha-ap8:151.4_0": "vinayo mama gocaro.",
  "tha-ap8:152.1_0": "Vinaye pāramippatto,",
  "tha-ap8:152.2_0": "samathe cāpi kovido;",
  "tha-ap8:152.3_0": "Upāli taṁ mahāvīra,",
  "tha-ap8:152.4_0": "pāde vandati satthuno.",
  "tha-ap8:153.1_0": "So ahaṁ vicarissāmi,",
  "tha-ap8:153.2_0": "gāmā gāmaṁ purā puraṁ;",
  "tha-ap8:153.3_0": "Namassamāno sambuddhaṁ,",
  "tha-ap8:153.4_0": "dhammassa ca sudhammataṁ.",
  "tha-ap8:154.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap8:154.2_0": "bhavā sabbe samūhatā;",
  "tha-ap8:154.3_0": "Sabbāsavā parikkhīṇā,",
  "tha-ap8:154.4_0": "natthi dāni punabbhavo.",
  "tha-ap8:155.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap8:155.2_0": "buddhaseṭṭhassa santike;",
  "tha-ap8:155.3_0": "Tisso vijjā anuppattā,",
  "tha-ap8:155.4_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap8:156.1_0": "Paṭisambhidā catasso,",
  "tha-ap8:156.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap8:156.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap8:156.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap8:157.1_0": "Itthaṁ sudaṁ āyasmā upāli thero imā gāthāyo abhāsitthāti.",
  "tha-ap8:158.1_0": "Upālittherassāpadānaṁ chaṭṭhaṁ."
}