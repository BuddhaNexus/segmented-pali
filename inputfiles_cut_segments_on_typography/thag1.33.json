{
  "thag1.33:0.1_0": "Theragāthā 1.33",
  "thag1.33:0.2_0": "Ekakanipāta",
  "thag1.33:0.3_0": "Catutthavagga",
  "thag1.33:0.4_0": "Sopākattheragāthā",
  "thag1.33:1.1_0": "“Yathāpi ekaputtasmiṁ,",
  "thag1.33:1.2_0": "piyasmiṁ kusalī siyā;",
  "thag1.33:1.3_0": "Evaṁ sabbesu pāṇesu,",
  "thag1.33:1.4_0": "sabbattha kusalo siyā”ti.",
  "thag1.33:1.5_0": "… Sopāko thero …."
}