{
  "tha-ap165:0.1_0": "Therāpadāna",
  "tha-ap165:0.2_0": "Supāricariyavagga",
  "tha-ap165:0.3_0": "3. Khajjakadāyakattheraapadāna",
  "tha-ap165:1.1_0": "“Tissassa kho bhagavato,",
  "tha-ap165:1.2_0": "pubbe phalamadāsahaṁ;",
  "tha-ap165:1.3_0": "Nāḷikerañca pādāsiṁ,",
  "tha-ap165:1.4_0": "khajjakaṁ abhisammataṁ.",
  "tha-ap165:2.1_0": "Buddhassa tamahaṁ datvā,",
  "tha-ap165:2.2_0": "Tissassa tu mahesino;",
  "tha-ap165:2.3_0": "Modāmahaṁ kāmakāmī,",
  "tha-ap165:2.4_0": "Upapajjiṁ yamicchakaṁ.",
  "tha-ap165:3.1_0": "Dvenavute ito kappe,",
  "tha-ap165:3.2_0": "yaṁ dānamadadiṁ tadā;",
  "tha-ap165:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap165:3.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap165:4.1_0": "Ito terasakappamhi,",
  "tha-ap165:4.2_0": "rājā indasamo ahu;",
  "tha-ap165:4.3_0": "Sattaratanasampanno,",
  "tha-ap165:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap165:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap165:5.2_0": "…pe…",
  "tha-ap165:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap165:6.1_0": "Itthaṁ sudaṁ āyasmā khajjakadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap165:7.1_0": "Khajjakadāyakattherassāpadānaṁ tatiyaṁ."
}