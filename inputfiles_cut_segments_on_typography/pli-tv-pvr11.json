{
  "pli-tv-pvr11:0.1_0": "Parivāra",
  "pli-tv-pvr11:0.2_0": "11. Adhikaraṇabheda",
  "pli-tv-pvr11:0.3_0": "1. Ukkoṭanabhedādi",
  "pli-tv-pvr11:1.1_0": "Cattāri adhikaraṇāni.",
  "pli-tv-pvr11:1.2_0": "Vivādādhikaraṇaṁ, anuvādādhikaraṇaṁ, āpattādhikaraṇaṁ, kiccādhikaraṇaṁ—",
  "pli-tv-pvr11:1.3_0": "imāni cattāri adhikaraṇāni.",
  "pli-tv-pvr11:2.1_0": "Imesaṁ catunnaṁ adhikaraṇānaṁ kati ukkoṭā?",
  "pli-tv-pvr11:2.2_0": "Imesaṁ catunnaṁ adhikaraṇānaṁ dasa ukkoṭā.",
  "pli-tv-pvr11:2.3_0": "Vivādādhikaraṇassa dve ukkoṭā,",
  "pli-tv-pvr11:2.3_1": "anuvādādhikaraṇassa cattāro ukkoṭā,",
  "pli-tv-pvr11:2.3_2": "āpattādhikaraṇassa tayo ukkoṭā,",
  "pli-tv-pvr11:2.3_3": "kiccādhikaraṇassa eko ukkoṭo—",
  "pli-tv-pvr11:2.4_0": "imesaṁ catunnaṁ adhikaraṇānaṁ ime dasa ukkoṭā.",
  "pli-tv-pvr11:3.1_0": "Vivādādhikaraṇaṁ ukkoṭento kati samathe ukkoṭeti?",
  "pli-tv-pvr11:3.2_0": "Anuvādādhikaraṇaṁ ukkoṭento kati samathe ukkoṭeti?",
  "pli-tv-pvr11:3.3_0": "Āpattādhikaraṇaṁ ukkoṭento kati samathe ukkoṭeti?",
  "pli-tv-pvr11:3.4_0": "Kiccādhikaraṇaṁ ukkoṭento kati samathe ukkoṭeti?",
  "pli-tv-pvr11:4.1_0": "Vivādādhikaraṇaṁ ukkoṭento dve samathe ukkoṭeti.",
  "pli-tv-pvr11:4.2_0": "Anuvādādhikaraṇaṁ ukkoṭento cattāro samathe ukkoṭeti.",
  "pli-tv-pvr11:4.3_0": "Āpattādhikaraṇaṁ ukkoṭento tayo samathe ukkoṭeti.",
  "pli-tv-pvr11:4.4_0": "Kiccādhikaraṇaṁ ukkoṭento ekaṁ samathaṁ ukkoṭeti.",
  "pli-tv-pvr11:5.1_0": "Kati ukkoṭā?",
  "pli-tv-pvr11:5.2_0": "Katihākārehi ukkoṭanaṁ pasavati?",
  "pli-tv-pvr11:5.3_0": "Katihaṅgehi samannāgato puggalo adhikaraṇaṁ ukkoṭeti?",
  "pli-tv-pvr11:5.4_0": "Kati puggalā adhikaraṇaṁ ukkoṭentā āpattiṁ āpajjanti?",
  "pli-tv-pvr11:6.1_0": "Dvādasa ukkoṭā.",
  "pli-tv-pvr11:6.2_0": "Dasahākārehi ukkoṭanaṁ pasavati.",
  "pli-tv-pvr11:6.3_0": "Catūhaṅgehi samannāgato puggalo adhikaraṇaṁ ukkoṭeti.",
  "pli-tv-pvr11:6.4_0": "Cattāro puggalā adhikaraṇaṁ ukkoṭentā āpattiṁ āpajjanti?",
  "pli-tv-pvr11:7.1_0": "Katame dvādasa ukkoṭā?",
  "pli-tv-pvr11:7.2_0": "Akataṁ kammaṁ,",
  "pli-tv-pvr11:7.2_1": "dukkaṭaṁ kammaṁ,",
  "pli-tv-pvr11:7.2_2": "puna kātabbaṁ kammaṁ, anihataṁ,",
  "pli-tv-pvr11:7.2_3": "dunnihataṁ,",
  "pli-tv-pvr11:7.2_4": "puna nihanitabbaṁ,",
  "pli-tv-pvr11:7.2_5": "avinicchitaṁ,",
  "pli-tv-pvr11:7.2_6": "duvinicchitaṁ,",
  "pli-tv-pvr11:7.2_7": "puna vinicchitabbaṁ,",
  "pli-tv-pvr11:7.2_8": "avūpasantaṁ,",
  "pli-tv-pvr11:7.2_9": "duvūpasantaṁ,",
  "pli-tv-pvr11:7.2_10": "puna vūpasametabbanti—",
  "pli-tv-pvr11:7.3_0": "ime dvādasa ukkoṭā.",
  "pli-tv-pvr11:8.1_0": "Katamehi dasahākārehi ukkoṭanaṁ pasavati?",
  "pli-tv-pvr11:8.2_0": "Tattha jātakaṁ adhikaraṇaṁ ukkoṭeti,",
  "pli-tv-pvr11:8.2_1": "tattha jātakaṁ vūpasantaṁ adhikaraṇaṁ ukkoṭeti,",
  "pli-tv-pvr11:8.2_2": "antarāmagge adhikaraṇaṁ ukkoṭeti,",
  "pli-tv-pvr11:8.2_3": "antarāmagge vūpasantaṁ adhikaraṇaṁ ukkoṭeti,",
  "pli-tv-pvr11:8.2_4": "tattha gataṁ adhikaraṇaṁ ukkoṭeti,",
  "pli-tv-pvr11:8.2_5": "tattha gataṁ vūpasantaṁ adhikaraṇaṁ ukkoṭeti,",
  "pli-tv-pvr11:8.2_6": "sativinayaṁ ukkoṭeti,",
  "pli-tv-pvr11:8.2_7": "amūḷhavinayaṁ ukkoṭeti,",
  "pli-tv-pvr11:8.2_8": "tassapāpiyasikaṁ ukkoṭeti,",
  "pli-tv-pvr11:8.2_9": "tiṇavatthārakaṁ ukkoṭeti—",
  "pli-tv-pvr11:8.3_0": "imehi dasahākārehi ukkoṭanaṁ pasavati.",
  "pli-tv-pvr11:9.1_0": "Katamehi catūhaṅgehi samannāgato puggalo adhikaraṇaṁ ukkoṭeti?",
  "pli-tv-pvr11:9.2_0": "Chandāgatiṁ gacchanto adhikaraṇaṁ ukkoṭeti,",
  "pli-tv-pvr11:9.2_1": "dosāgatiṁ gacchanto adhikaraṇaṁ ukkoṭeti,",
  "pli-tv-pvr11:9.2_2": "mohāgatiṁ gacchanto adhikaraṇaṁ ukkoṭeti,",
  "pli-tv-pvr11:9.2_3": "bhayāgatiṁ gacchanto adhikaraṇaṁ ukkoṭeti—",
  "pli-tv-pvr11:9.3_0": "imehi catūhaṅgehi samannāgato puggalo adhikaraṇaṁ ukkoṭeti.",
  "pli-tv-pvr11:10.1_0": "Katame cattāro puggalā adhikaraṇaṁ ukkoṭentā āpattiṁ āpajjanti?",
  "pli-tv-pvr11:10.2_0": "Tadahupasampanno ukkoṭeti ukkoṭanakaṁ pācittiyaṁ,",
  "pli-tv-pvr11:10.2_1": "āgantuko ukkoṭeti ukkoṭanakaṁ pācittiyaṁ,",
  "pli-tv-pvr11:10.2_2": "kārako ukkoṭeti ukkoṭanakaṁ pācittiyaṁ,",
  "pli-tv-pvr11:10.2_3": "chandadāyako ukkoṭeti ukkoṭanakaṁ pācittiyaṁ—",
  "pli-tv-pvr11:10.3_0": "ime cattāro puggalā adhikaraṇaṁ ukkoṭentā āpattiṁ āpajjanti.",
  "pli-tv-pvr11:11.0_0": "2. Adhikaraṇanidānādi",
  "pli-tv-pvr11:11.1_0": "Vivādādhikaraṇaṁ kiṁnidānaṁ, kiṁsamudayaṁ, kiṁjātikaṁ, kiṁpabhavaṁ, kiṁsambhāraṁ, kiṁsamuṭṭhānaṁ?",
  "pli-tv-pvr11:11.2_0": "Anuvādādhikaraṇaṁ kiṁnidānaṁ, kiṁsamudayaṁ, kiṁjātikaṁ, kiṁpabhavaṁ, kiṁsambhāraṁ, kiṁsamuṭṭhānaṁ?",
  "pli-tv-pvr11:11.3_0": "Āpattādhikaraṇaṁ kiṁnidānaṁ, kiṁsamudayaṁ, kiṁjātikaṁ, kiṁpabhavaṁ, kiṁsambhāraṁ, kiṁsamuṭṭhānaṁ?",
  "pli-tv-pvr11:11.4_0": "Kiccādhikaraṇaṁ kiṁnidānaṁ, kiṁsamudayaṁ, kiṁjātikaṁ, kiṁpabhavaṁ, kiṁsambhāraṁ, kiṁsamuṭṭhānaṁ?",
  "pli-tv-pvr11:12.1_0": "Vivādādhikaraṇaṁ vivādanidānaṁ vivādasamudayaṁ vivādajātikaṁ vivādapabhavaṁ vivādasambhāraṁ vivādasamuṭṭhānaṁ.",
  "pli-tv-pvr11:12.2_0": "Anuvādādhikaraṇaṁ anuvādanidānaṁ anuvādasamudayaṁ anuvādajātikaṁ anuvādapabhavaṁ anuvādasambhāraṁ anuvādasamuṭṭhānaṁ.",
  "pli-tv-pvr11:12.3_0": "Āpattādhikaraṇaṁ āpattinidānaṁ āpattisamudayaṁ āpattijātikaṁ āpattipabhavaṁ āpattisambhāraṁ āpattisamuṭṭhānaṁ.",
  "pli-tv-pvr11:12.4_0": "Kiccādhikaraṇaṁ kiccayanidānaṁ kiccayasamudayaṁ kiccayajātikaṁ kiccayapabhavaṁ kiccayasambhāraṁ kiccayasamuṭṭhānaṁ.",
  "pli-tv-pvr11:13.1_0": "Vivādādhikaraṇaṁ kiṁnidānaṁ, kiṁsamudayaṁ, kiṁjātikaṁ, kiṁpabhavaṁ, kiṁsambhāraṁ, kiṁsamuṭṭhānaṁ?",
  "pli-tv-pvr11:13.2_0": "Anuvādādhikaraṇaṁ …pe…",
  "pli-tv-pvr11:13.3_0": "āpattādhikaraṇaṁ …pe…",
  "pli-tv-pvr11:13.4_0": "kiccādhikaraṇaṁ kiṁnidānaṁ, kiṁsamudayaṁ, kiṁjātikaṁ, kiṁpabhavaṁ, kiṁsambhāraṁ, kiṁsamuṭṭhānaṁ?",
  "pli-tv-pvr11:14.1_0": "Vivādādhikaraṇaṁ hetunidānaṁ,",
  "pli-tv-pvr11:14.1_1": "hetusamudayaṁ,",
  "pli-tv-pvr11:14.1_2": "hetujātikaṁ,",
  "pli-tv-pvr11:14.1_3": "hetupabhavaṁ,",
  "pli-tv-pvr11:14.1_4": "hetusambhāraṁ,",
  "pli-tv-pvr11:14.1_5": "hetusamuṭṭhānaṁ.",
  "pli-tv-pvr11:14.2_0": "Anuvādādhikaraṇaṁ …pe…",
  "pli-tv-pvr11:14.3_0": "āpattādhikaraṇaṁ …pe…",
  "pli-tv-pvr11:14.4_0": "kiccādhikaraṇaṁ hetunidānaṁ,",
  "pli-tv-pvr11:14.4_1": "hetusamudayaṁ,",
  "pli-tv-pvr11:14.4_2": "hetujātikaṁ,",
  "pli-tv-pvr11:14.4_3": "hetupabhavaṁ,",
  "pli-tv-pvr11:14.4_4": "hetusambhāraṁ,",
  "pli-tv-pvr11:14.4_5": "hetusamuṭṭhānaṁ.",
  "pli-tv-pvr11:15.1_0": "Vivādādhikaraṇaṁ kiṁnidānaṁ, kiṁsamudayaṁ, kiṁjātikaṁ, kiṁpabhavaṁ, kiṁsambhāraṁ, kiṁsamuṭṭhānaṁ?",
  "pli-tv-pvr11:15.2_0": "Anuvādādhikaraṇaṁ …pe…",
  "pli-tv-pvr11:15.3_0": "āpattādhikaraṇaṁ …pe…",
  "pli-tv-pvr11:15.4_0": "kiccādhikaraṇaṁ kiṁnidānaṁ, kiṁsamudayaṁ, kiṁjātikaṁ, kiṁpabhavaṁ, kiṁsambhāraṁ, kiṁsamuṭṭhānaṁ?",
  "pli-tv-pvr11:16.1_0": "Vivādādhikaraṇaṁ paccayanidānaṁ,",
  "pli-tv-pvr11:16.1_1": "paccayasamudayaṁ,",
  "pli-tv-pvr11:16.1_2": "paccayajātikaṁ,",
  "pli-tv-pvr11:16.1_3": "paccayapabhavaṁ,",
  "pli-tv-pvr11:16.1_4": "paccayasambhāraṁ,",
  "pli-tv-pvr11:16.1_5": "paccayasamuṭṭhānaṁ.",
  "pli-tv-pvr11:16.2_0": "Anuvādādhikaraṇaṁ …pe…",
  "pli-tv-pvr11:16.3_0": "āpattādhikaraṇaṁ …pe…",
  "pli-tv-pvr11:16.4_0": "kiccādhikaraṇaṁ paccayanidānaṁ,",
  "pli-tv-pvr11:16.4_1": "paccayasamudayaṁ,",
  "pli-tv-pvr11:16.4_2": "paccayajātikaṁ,",
  "pli-tv-pvr11:16.4_3": "paccayapabhavaṁ,",
  "pli-tv-pvr11:16.4_4": "paccayasambhāraṁ,",
  "pli-tv-pvr11:16.4_5": "paccayasamuṭṭhānaṁ.",
  "pli-tv-pvr11:17.0_0": "3. Adhikaraṇamūlādi",
  "pli-tv-pvr11:17.1_0": "Catunnaṁ adhikaraṇānaṁ kati mūlāni, kati samuṭṭhānā?",
  "pli-tv-pvr11:17.2_0": "Catunnaṁ adhikaraṇānaṁ tettiṁsa mūlāni, tettiṁsa samuṭṭhānā.",
  "pli-tv-pvr11:18.1_0": "Catunnaṁ adhikaraṇānaṁ katamāni tettiṁsa mūlāni?",
  "pli-tv-pvr11:18.2_0": "Vivādādhikaraṇassa dvādasa mūlāni,",
  "pli-tv-pvr11:18.2_1": "anuvādādhikaraṇassa cuddasa mūlāni,",
  "pli-tv-pvr11:18.2_2": "āpattādhikaraṇassa cha mūlāni,",
  "pli-tv-pvr11:18.2_3": "kiccādhikaraṇassa ekaṁ mūlaṁ, saṅgho—",
  "pli-tv-pvr11:18.3_0": "catunnaṁ adhikaraṇānaṁ imāni tettiṁsa mūlāni.",
  "pli-tv-pvr11:19.1_0": "Catunnaṁ adhikaraṇānaṁ katame tettiṁsa samuṭṭhānā?",
  "pli-tv-pvr11:19.2_0": "Vivādādhikaraṇassa aṭṭhārasabhedakaravatthūni samuṭṭhānā,",
  "pli-tv-pvr11:19.2_1": "anuvādādhikaraṇassa catasso vipattiyo samuṭṭhānā,",
  "pli-tv-pvr11:19.2_2": "āpattādhikaraṇassa sattāpattikkhandhā samuṭṭhānā,",
  "pli-tv-pvr11:19.2_3": "kiccādhikaraṇassa cattāri kammāni samuṭṭhānā—",
  "pli-tv-pvr11:19.3_0": "catunnaṁ adhikaraṇānaṁ ime tettiṁsa samuṭṭhānā.",
  "pli-tv-pvr11:20.0_0": "4. Adhikaraṇapaccayāpatti",
  "pli-tv-pvr11:20.1_0": "Vivādādhikaraṇaṁ āpattānāpattīti?",
  "pli-tv-pvr11:20.2_0": "Vivādādhikaraṇaṁ na āpatti.",
  "pli-tv-pvr11:20.3_0": "Kiṁ pana vivādādhikaraṇapaccayā āpattiṁ āpajjeyyāti?",
  "pli-tv-pvr11:20.4_0": "Āma, vivādādhikaraṇapaccayā āpattiṁ āpajjeyya.",
  "pli-tv-pvr11:20.5_0": "Vivādādhikaraṇapaccayā kati āpattiyo āpajjati?",
  "pli-tv-pvr11:20.6_0": "Vivādādhikaraṇapaccayā dve āpattiyo āpajjati.",
  "pli-tv-pvr11:20.7_0": "Upasampannaṁ omasati, āpatti pācittiyassa;",
  "pli-tv-pvr11:20.8_0": "anupasampannaṁ omasati, āpatti dukkaṭassa—",
  "pli-tv-pvr11:20.9_0": "vivādādhikaraṇapaccayā imā dve āpattiyo āpajjati.",
  "pli-tv-pvr11:21.1_0": "Tā āpattiyo catunnaṁ vipattīnaṁ kati vipattiyo bhajanti?",
  "pli-tv-pvr11:21.2_0": "Catunnaṁ adhikaraṇānaṁ katamaṁ adhikaraṇaṁ?",
  "pli-tv-pvr11:21.3_0": "Sattannaṁ āpattikkhandhānaṁ katihi āpattikkhandhehi saṅgahitā?",
  "pli-tv-pvr11:21.4_0": "Channaṁ āpattisamuṭṭhānānaṁ katihi samuṭṭhānehi samuṭṭhanti?",
  "pli-tv-pvr11:21.5_0": "Katihi adhikaraṇehi, katisu ṭhānesu, katihi samathehi sammanti?",
  "pli-tv-pvr11:22.1_0": "Tā āpattiyo catunnaṁ vipattīnaṁ ekaṁ vipattiṁ bhajanti—",
  "pli-tv-pvr11:22.2_0": "ācāravipattiṁ.",
  "pli-tv-pvr11:22.3_0": "Catunnaṁ adhikaraṇānaṁ, āpattādhikaraṇaṁ.",
  "pli-tv-pvr11:22.4_0": "Sattannaṁ āpattikkhandhānaṁ dvīhi āpattikkhandhehi saṅgahitā—",
  "pli-tv-pvr11:22.5_0": "siyā pācittiyāpattikkhandhena, siyā dukkaṭāpattikkhandhena.",
  "pli-tv-pvr11:22.6_0": "Channaṁ āpattisamuṭṭhānānaṁ tīhi samuṭṭhānehi samuṭṭhanti.",
  "pli-tv-pvr11:22.7_0": "Ekena adhikaraṇena—",
  "pli-tv-pvr11:22.8_0": "kiccādhikaraṇena;",
  "pli-tv-pvr11:22.9_0": "tīsu ṭhānesu—",
  "pli-tv-pvr11:22.10_0": "saṅghamajjhe, gaṇamajjhe, puggalassa santike;",
  "pli-tv-pvr11:22.11_0": "tīhi samathehi sammanti—",
  "pli-tv-pvr11:22.12_0": "siyā sammukhāvinayena ca paṭiññātakaraṇena ca, siyā sammukhāvinayena ca tiṇavatthārakena ca.",
  "pli-tv-pvr11:23.1_0": "Anuvādādhikaraṇaṁ āpattānāpattīti?",
  "pli-tv-pvr11:23.2_0": "Anuvādādhikaraṇaṁ na āpatti.",
  "pli-tv-pvr11:23.3_0": "Kiṁ pana anuvādādhikaraṇapaccayā āpattiṁ āpajjeyyāti?",
  "pli-tv-pvr11:23.4_0": "Āma, anuvādādhikaraṇapaccayā āpattiṁ āpajjeyya.",
  "pli-tv-pvr11:23.5_0": "Anuvādādhikaraṇapaccayā, kati āpattiyo āpajjati?",
  "pli-tv-pvr11:23.6_0": "Anuvādādhikaraṇapaccayā tisso āpattiyo āpajjati.",
  "pli-tv-pvr11:23.7_0": "Bhikkhuṁ amūlakena pārājikena dhammena anuddhaṁseti, āpatti saṅghādisesassa;",
  "pli-tv-pvr11:23.8_0": "amūlakena saṅghādisesena anuddhaṁseti, āpatti pācittiyassa;",
  "pli-tv-pvr11:23.9_0": "amūlikāya ācāravipattiyā anuddhaṁseti, āpatti dukkaṭassa—",
  "pli-tv-pvr11:23.10_0": "anuvādādhikaraṇapaccayā imā tisso āpattiyo āpajjati.",
  "pli-tv-pvr11:24.1_0": "Tā āpattiyo catunnaṁ vipattīnaṁ kati vipattiyo bhajanti?",
  "pli-tv-pvr11:24.2_0": "Catunnaṁ adhikaraṇānaṁ katamaṁ adhikaraṇaṁ?",
  "pli-tv-pvr11:24.3_0": "Sattannaṁ āpattikkhandhānaṁ katihi āpattikkhandhehi saṅgahitā?",
  "pli-tv-pvr11:24.4_0": "Channaṁ āpattisamuṭṭhānānaṁ katihi samuṭṭhānehi samuṭṭhanti?",
  "pli-tv-pvr11:24.5_0": "Katihi adhikaraṇehi, katisu ṭhānesu, katihi samathehi sammanti?",
  "pli-tv-pvr11:25.1_0": "Tā āpattiyo catunnaṁ vipattīnaṁ dve vipattiyo bhajanti—",
  "pli-tv-pvr11:25.2_0": "siyā sīlavipattiṁ, siyā ācāravipattiṁ.",
  "pli-tv-pvr11:25.3_0": "Catunnaṁ adhikaraṇānaṁ, āpattādhikaraṇaṁ.",
  "pli-tv-pvr11:25.4_0": "Sattannaṁ āpattikkhandhānaṁ tīhi āpattikkhandhehi saṅgahitā—",
  "pli-tv-pvr11:25.5_0": "siyā saṅghādisesāpattikkhandhena, siyā pācittiyāpattikkhandhena, siyā dukkaṭāpattikkhandhena.",
  "pli-tv-pvr11:25.6_0": "Channaṁ āpattisamuṭṭhānānaṁ tīhi samuṭṭhānehi samuṭṭhanti.",
  "pli-tv-pvr11:25.7_0": "Yā tā āpattiyo garukā tā āpattiyo ekena adhikaraṇena—",
  "pli-tv-pvr11:25.8_0": "kiccādhikaraṇena;",
  "pli-tv-pvr11:25.9_0": "ekamhi ṭhāne—",
  "pli-tv-pvr11:25.10_0": "saṅghamajjhe;",
  "pli-tv-pvr11:25.11_0": "dvīhi samathehi sammanti—",
  "pli-tv-pvr11:25.12_0": "sammukhāvinayena ca paṭiññātakaraṇena ca.",
  "pli-tv-pvr11:25.13_0": "Yā tā āpattiyo lahukā tā āpattiyo ekena adhikaraṇena—",
  "pli-tv-pvr11:25.14_0": "kiccādhikaraṇena;",
  "pli-tv-pvr11:25.15_0": "tīsu ṭhānesu—",
  "pli-tv-pvr11:25.16_0": "saṅghamajjhe gaṇamajjhe puggalassa santike;",
  "pli-tv-pvr11:25.17_0": "tīhi samathehi sammanti—",
  "pli-tv-pvr11:25.18_0": "siyā sammukhāvinayena ca paṭiññātakaraṇena ca, siyā sammukhāvinayena ca tiṇavatthārakena ca.",
  "pli-tv-pvr11:26.1_0": "Āpattādhikaraṇaṁ āpattānāpattīti?",
  "pli-tv-pvr11:26.2_0": "Āpattādhikaraṇaṁ āpatti.",
  "pli-tv-pvr11:26.3_0": "Kiṁ pana āpattādhikaraṇapaccayā āpattiṁ āpajjeyyāti?",
  "pli-tv-pvr11:26.4_0": "Āma, āpattādhikaraṇapaccayā āpattiṁ āpajjeyya.",
  "pli-tv-pvr11:26.5_0": "Āpattādhikaraṇapaccayā kati āpattiyo āpajjati?",
  "pli-tv-pvr11:26.6_0": "Āpattādhikaraṇapaccayā catasso āpattiyo āpajjati.",
  "pli-tv-pvr11:26.7_0": "Bhikkhunī jānaṁ pārājikaṁ dhammaṁ paṭicchādeti, āpatti pārājikassa;",
  "pli-tv-pvr11:26.8_0": "vematikā paṭicchādeti, āpatti thullaccayassa;",
  "pli-tv-pvr11:26.9_0": "bhikkhu saṅghādisesaṁ paṭicchādeti, āpatti pācittiyassa;",
  "pli-tv-pvr11:26.10_0": "ācāravipattiṁ paṭicchādeti, āpatti dukkaṭassa—",
  "pli-tv-pvr11:26.11_0": "āpattādhikaraṇapaccayā imā catasso āpattiyo āpajjati.",
  "pli-tv-pvr11:27.1_0": "Tā āpattiyo catunnaṁ vipattīnaṁ kati vipattiyo bhajanti?",
  "pli-tv-pvr11:27.2_0": "Catunnaṁ adhikaraṇānaṁ katamaṁ adhikaraṇaṁ?",
  "pli-tv-pvr11:27.3_0": "Sattannaṁ āpattikkhandhānaṁ katihi āpattikkhandhehi saṅgahitā?",
  "pli-tv-pvr11:27.4_0": "Channaṁ āpattisamuṭṭhānānaṁ katihi samuṭṭhānehi samuṭṭhanti?",
  "pli-tv-pvr11:27.5_0": "Katihi adhikaraṇehi, katisu ṭhānesu, katihi samathehi sammanti?",
  "pli-tv-pvr11:28.1_0": "Tā āpattiyo catunnaṁ vipattīnaṁ dve vipattiyo bhajanti—",
  "pli-tv-pvr11:28.2_0": "siyā sīlavipattiṁ siyā ācāravipattiṁ.",
  "pli-tv-pvr11:28.3_0": "Catunnaṁ adhikaraṇānaṁ āpattādhikaraṇaṁ.",
  "pli-tv-pvr11:28.4_0": "Sattannaṁ āpattikkhandhānaṁ catūhi āpattikkhandhehi saṅgahitā—",
  "pli-tv-pvr11:28.5_0": "siyā pārājikāpattikkhandhena,",
  "pli-tv-pvr11:28.5_1": "siyā thullaccayāpattikkhandhena,",
  "pli-tv-pvr11:28.5_2": "siyā pācittiyāpattikkhandhena,",
  "pli-tv-pvr11:28.5_3": "siyā dukkaṭāpattikkhandhena.",
  "pli-tv-pvr11:28.6_0": "Channaṁ āpattisamuṭṭhānānaṁ ekena samuṭṭhānena samuṭṭhanti—",
  "pli-tv-pvr11:28.7_0": "kāyato ca vācato ca cittato ca samuṭṭhanti.",
  "pli-tv-pvr11:28.8_0": "Yā sā āpatti anavasesā sā āpatti na katamena adhikaraṇena,",
  "pli-tv-pvr11:28.8_1": "na katamamhi ṭhāne,",
  "pli-tv-pvr11:28.8_2": "na katamena samathena sammati.",
  "pli-tv-pvr11:28.9_0": "Yā tā āpattiyo lahukā tā āpattiyo ekena adhikaraṇena—",
  "pli-tv-pvr11:28.10_0": "kiccādhikaraṇena;",
  "pli-tv-pvr11:28.11_0": "tīsu ṭhānesu—",
  "pli-tv-pvr11:28.12_0": "saṅghamajjhe, gaṇamajjhe, puggalassa santike;",
  "pli-tv-pvr11:28.13_0": "tīhi samathehi sammanti—",
  "pli-tv-pvr11:28.14_0": "siyā sammukhāvinayena ca paṭiññātakaraṇena ca, siyā sammukhāvinayena ca tiṇavatthārakena ca.",
  "pli-tv-pvr11:29.1_0": "Kiccādhikaraṇaṁ āpattānāpattīti?",
  "pli-tv-pvr11:29.2_0": "Kiccādhikaraṇaṁ na āpatti.",
  "pli-tv-pvr11:29.3_0": "Kiṁ pana kiccādhikaraṇapaccayā āpattiṁ āpajjeyyāti?",
  "pli-tv-pvr11:29.4_0": "Āma, kiccādhikaraṇapaccayā āpattiṁ āpajjeyya.",
  "pli-tv-pvr11:29.5_0": "Kiccādhikaraṇapaccayā kati āpattiyo āpajjati?",
  "pli-tv-pvr11:29.6_0": "Kiccādhikaraṇapaccayā pañca āpattiyo āpajjati.",
  "pli-tv-pvr11:29.7_0": "Ukkhittānuvattikā bhikkhunī yāvatatiyaṁ samanubhāsanāya na paṭinissajjati, ñattiyā dukkaṭaṁ;",
  "pli-tv-pvr11:29.8_0": "dvīhi kammavācāhi thullaccayā;",
  "pli-tv-pvr11:29.9_0": "kammavācāpariyosāne āpatti pārājikassa;",
  "pli-tv-pvr11:29.10_0": "bhedakānuvattakā bhikkhū yāvatatiyaṁ samanubhāsanāya na paṭinissajjanti, āpatti saṅghādisesassa;",
  "pli-tv-pvr11:29.11_0": "pāpikāya diṭṭhiyā yāvatatiyaṁ samanubhāsanāya na paṭinissajjanti, āpatti pācittiyassa—",
  "pli-tv-pvr11:29.12_0": "kiccādhikaraṇapaccayā imā pañca āpattiyo āpajjati.",
  "pli-tv-pvr11:30.1_0": "Tā āpattiyo catunnaṁ vipattīnaṁ kati vipattiyo bhajanti?",
  "pli-tv-pvr11:30.2_0": "Catunnaṁ adhikaraṇānaṁ katamaṁ adhikaraṇaṁ?",
  "pli-tv-pvr11:30.3_0": "Sattannaṁ āpattikkhandhānaṁ katihi āpattikkhandhehi saṅgahitā?",
  "pli-tv-pvr11:30.4_0": "Channaṁ āpattisamuṭṭhānānaṁ katihi samuṭṭhānehi samuṭṭhanti?",
  "pli-tv-pvr11:30.5_0": "Katihi adhikaraṇehi, katisu ṭhānesu, katihi samathehi sammanti?",
  "pli-tv-pvr11:31.1_0": "Tā āpattiyo catunnaṁ vipattīnaṁ dve vipattiyo bhajanti—",
  "pli-tv-pvr11:31.2_0": "siyā sīlavipattiṁ siyā ācāravipattiṁ.",
  "pli-tv-pvr11:31.3_0": "Catunnaṁ adhikaraṇānaṁ āpattādhikaraṇaṁ.",
  "pli-tv-pvr11:31.4_0": "Sattannaṁ āpattikkhandhānaṁ pañcahi āpattikkhandhehi saṅgahitā—",
  "pli-tv-pvr11:31.5_0": "siyā pārājikāpattikkhandhena,",
  "pli-tv-pvr11:31.5_1": "siyā saṅghādisesāpattikkhandhena,",
  "pli-tv-pvr11:31.5_2": "siyā thullaccayāpattikkhandhena,",
  "pli-tv-pvr11:31.5_3": "siyā pācittiyāpattikkhandhena,",
  "pli-tv-pvr11:31.5_4": "siyā dukkaṭāpattikkhandhena.",
  "pli-tv-pvr11:31.6_0": "Channaṁ āpattisamuṭṭhānānaṁ ekena samuṭṭhānena samuṭṭhanti—",
  "pli-tv-pvr11:31.7_0": "kāyato ca vācato ca cittato ca samuṭṭhanti.",
  "pli-tv-pvr11:31.8_0": "Yā sā āpatti anavasesā sā āpatti na katamena adhikaraṇena,",
  "pli-tv-pvr11:31.8_1": "na katamamhi ṭhāne,",
  "pli-tv-pvr11:31.8_2": "na katamena samathena sammati.",
  "pli-tv-pvr11:31.9_0": "Yā sā āpatti garukā sā āpatti ekena adhikaraṇena—",
  "pli-tv-pvr11:31.10_0": "kiccādhikaraṇena;",
  "pli-tv-pvr11:31.11_0": "ekamhi ṭhāne—",
  "pli-tv-pvr11:31.12_0": "saṅghamajjhe;",
  "pli-tv-pvr11:31.13_0": "dvīhi samathehi sammati—",
  "pli-tv-pvr11:31.14_0": "sammukhāvinayena ca paṭiññātakaraṇena ca.",
  "pli-tv-pvr11:31.15_0": "Yā tā āpattiyo lahukā tā āpattiyo ekena adhikaraṇena—",
  "pli-tv-pvr11:31.16_0": "kiccādhikaraṇena;",
  "pli-tv-pvr11:31.17_0": "tīsu ṭhānesu—",
  "pli-tv-pvr11:31.18_0": "saṅghamajjhe, gaṇamajjhe, puggalassa santike;",
  "pli-tv-pvr11:31.19_0": "tīhi samathehi sammanti—",
  "pli-tv-pvr11:31.20_0": "siyā sammukhāvinayena ca paṭiññātakaraṇena ca, siyā sammukhāvinayena ca tiṇavatthārakena ca.",
  "pli-tv-pvr11:32.0_0": "5. Adhikaraṇādhippāya",
  "pli-tv-pvr11:32.1_0": "Vivādādhikaraṇaṁ hoti anuvādādhikaraṇaṁ, hoti āpattādhikaraṇaṁ, hoti kiccādhikaraṇaṁ.",
  "pli-tv-pvr11:32.2_0": "Vivādādhikaraṇaṁ na hoti anuvādādhikaraṇaṁ, na hoti āpattādhikaraṇaṁ, na hoti kiccādhikaraṇaṁ;",
  "pli-tv-pvr11:32.3_0": "api ca, vivādādhikaraṇapaccayā hoti anuvādādhikaraṇaṁ, hoti āpattādhikaraṇaṁ, hoti kiccādhikaraṇaṁ.",
  "pli-tv-pvr11:32.4_0": "Yathā kathaṁ viya?",
  "pli-tv-pvr11:32.5_0": "Idha bhikkhū vivadanti—",
  "pli-tv-pvr11:32.6_0": "dhammoti vā adhammoti vā …pe…",
  "pli-tv-pvr11:32.7_0": "duṭṭhullāpattīti vā aduṭṭhullāpattīti vā.",
  "pli-tv-pvr11:32.8_0": "Yaṁ tattha bhaṇḍanaṁ kalaho viggaho vivādo nānāvādo aññathāvādo vipaccatāya vohāro medhakaṁ,",
  "pli-tv-pvr11:32.8_1": "idaṁ vuccati vivādādhikaraṇaṁ.",
  "pli-tv-pvr11:32.9_0": "Vivādādhikaraṇe saṅgho vivadati vivādādhikaraṇaṁ.",
  "pli-tv-pvr11:32.10_0": "Vivadamāno anuvadati anuvādādhikaraṇaṁ.",
  "pli-tv-pvr11:32.11_0": "Anuvadamāno āpattiṁ āpajjati āpattādhikaraṇaṁ.",
  "pli-tv-pvr11:32.12_0": "Tāya āpattiyā saṅgho kammaṁ karoti kiccādhikaraṇaṁ.",
  "pli-tv-pvr11:32.13_0": "Evaṁ vivādādhikaraṇapaccayā hoti anuvādādhikaraṇaṁ, hoti āpattādhikaraṇaṁ, hoti kiccādhikaraṇaṁ.",
  "pli-tv-pvr11:33.1_0": "Anuvādādhikaraṇaṁ hoti āpattādhikaraṇaṁ, hoti kiccādhikaraṇaṁ, hoti vivādādhikaraṇaṁ.",
  "pli-tv-pvr11:33.2_0": "Anuvādādhikaraṇaṁ na hoti āpattādhikaraṇaṁ, na hoti kiccādhikaraṇaṁ, na hoti vivādādhikaraṇaṁ;",
  "pli-tv-pvr11:33.3_0": "api ca anuvādādhikaraṇapaccayā hoti āpattādhikaraṇaṁ, hoti kiccādhikaraṇaṁ, hoti vivādādhikaraṇaṁ.",
  "pli-tv-pvr11:33.4_0": "Yathā kathaṁ viya?",
  "pli-tv-pvr11:33.5_0": "Idha bhikkhū bhikkhuṁ anuvadanti sīlavipattiyā vā ācāravipattiyā vā diṭṭhivipattiyā vā ājīvavipattiyā vā.",
  "pli-tv-pvr11:33.6_0": "Yo tattha anuvādo anuvadanā anullapanā anubhaṇanā anusampavaṅkatā abbhussahanatā anubalappadānaṁ,",
  "pli-tv-pvr11:33.6_1": "idaṁ vuccati anuvādādhikaraṇaṁ.",
  "pli-tv-pvr11:33.7_0": "Anuvādādhikaraṇe saṅgho vivadati, vivādādhikaraṇaṁ.",
  "pli-tv-pvr11:33.8_0": "Vivadamāno anuvadati, anuvādādhikaraṇaṁ.",
  "pli-tv-pvr11:33.9_0": "Anuvadamāno āpattiṁ āpajjati, āpattādhikaraṇaṁ.",
  "pli-tv-pvr11:33.10_0": "Tāya āpattiyā saṅgho kammaṁ karoti, kiccādhikaraṇaṁ.",
  "pli-tv-pvr11:33.11_0": "Evaṁ anuvādādhikaraṇapaccayā hoti āpattādhikaraṇaṁ, hoti kiccādhikaraṇaṁ, hoti vivādādhikaraṇaṁ.",
  "pli-tv-pvr11:34.1_0": "Āpattādhikaraṇaṁ hoti kiccādhikaraṇaṁ, hoti vivādādhikaraṇaṁ, hoti anuvādādhikaraṇaṁ.",
  "pli-tv-pvr11:34.2_0": "Āpattādhikaraṇaṁ na hoti kiccādhikaraṇaṁ, na hoti vivādādhikaraṇaṁ, na hoti anuvādādhikaraṇaṁ;",
  "pli-tv-pvr11:34.3_0": "api ca āpattādhikaraṇapaccayā hoti kiccādhikaraṇaṁ, hoti vivādādhikaraṇaṁ, hoti anuvādādhikaraṇaṁ.",
  "pli-tv-pvr11:34.4_0": "Yathā kathaṁ viya?",
  "pli-tv-pvr11:34.5_0": "Pañcapi āpattikkhandhā āpattādhikaraṇaṁ,",
  "pli-tv-pvr11:34.5_1": "sattapi āpattikkhandhā āpattādhikaraṇaṁ,",
  "pli-tv-pvr11:34.5_2": "idaṁ vuccati āpattādhikaraṇaṁ.",
  "pli-tv-pvr11:34.6_0": "Āpattādhikaraṇe saṅgho vivadati vivādādhikaraṇaṁ.",
  "pli-tv-pvr11:34.7_0": "Vivadamāno anuvadati anuvādādhikaraṇaṁ.",
  "pli-tv-pvr11:34.8_0": "Anuvadamāno āpattiṁ āpajjati āpattādhikaraṇaṁ.",
  "pli-tv-pvr11:34.9_0": "Tāya āpattiyā saṅgho kammaṁ karoti kiccādhikaraṇaṁ.",
  "pli-tv-pvr11:34.10_0": "Evaṁ āpattādhikaraṇapaccayā hoti kiccādhikaraṇaṁ, hoti vivādādhikaraṇaṁ, hoti anuvādādhikaraṇaṁ.",
  "pli-tv-pvr11:35.1_0": "Kiccādhikaraṇaṁ hoti vivādādhikaraṇaṁ, hoti anuvādādhikaraṇaṁ, hoti āpattādhikaraṇaṁ.",
  "pli-tv-pvr11:35.2_0": "Kiccādhikaraṇaṁ na hoti vivādādhikaraṇaṁ, na hoti anuvādādhikaraṇaṁ, na hoti āpattādhikaraṇaṁ;",
  "pli-tv-pvr11:35.3_0": "api ca kiccādhikaraṇapaccayā hoti vivādādhikaraṇaṁ, hoti anuvādādhikaraṇaṁ, hoti āpattādhikaraṇaṁ.",
  "pli-tv-pvr11:35.4_0": "Yathā kathaṁ viya?",
  "pli-tv-pvr11:35.5_0": "Yā saṅghassa kiccayatā karaṇīyatā apalokanakammaṁ ñattikammaṁ ñattidutiyakammaṁ ñatticatutthakammaṁ,",
  "pli-tv-pvr11:35.5_1": "idaṁ vuccati kiccādhikaraṇaṁ.",
  "pli-tv-pvr11:35.6_0": "Kiccādhikaraṇe saṅgho vivadati vivādādhikaraṇaṁ.",
  "pli-tv-pvr11:35.7_0": "Vivadamāno anuvadati anuvādādhikaraṇaṁ.",
  "pli-tv-pvr11:35.8_0": "Anuvadamāno āpattiṁ āpajjati āpattādhikaraṇaṁ.",
  "pli-tv-pvr11:35.9_0": "Tāya āpattiyā saṅgho kammaṁ karoti kiccādhikaraṇaṁ.",
  "pli-tv-pvr11:35.10_0": "Evaṁ kiccādhikaraṇapaccayā hoti vivādādhikaraṇaṁ, hoti anuvādādhikaraṇaṁ, hoti āpattādhikaraṇaṁ.",
  "pli-tv-pvr11:36.0_0": "6. Pucchāvāra",
  "pli-tv-pvr11:36.1_0": "Yattha sativinayo tattha sammukhāvinayo?",
  "pli-tv-pvr11:36.2_0": "Yattha sammukhāvinayo tattha sativinayo?",
  "pli-tv-pvr11:36.3_0": "Yattha amūḷhavinayo tattha sammukhāvinayo?",
  "pli-tv-pvr11:36.4_0": "Yattha sammukhāvinayo tattha amūḷhavinayo?",
  "pli-tv-pvr11:36.5_0": "Yattha paṭiññātakaraṇaṁ tattha sammukhāvinayo?",
  "pli-tv-pvr11:36.6_0": "Yattha sammukhāvinayo tattha paṭiññātakaraṇaṁ?",
  "pli-tv-pvr11:36.7_0": "Yattha yebhuyyasikā tattha sammukhāvinayo?",
  "pli-tv-pvr11:36.8_0": "Yattha sammukhāvinayo tattha yebhuyyasikā?",
  "pli-tv-pvr11:36.9_0": "Yattha tassapāpiyasikā tattha sammukhāvinayo?",
  "pli-tv-pvr11:36.10_0": "Yattha sammukhāvinayo tattha tassapāpiyasikā?",
  "pli-tv-pvr11:36.11_0": "Yattha tiṇavatthārako tattha sammukhāvinayo?",
  "pli-tv-pvr11:36.12_0": "Yattha sammukhāvinayo tattha tiṇavatthārako?",
  "pli-tv-pvr11:37.0_0": "7. Vissajjanāvāra",
  "pli-tv-pvr11:37.1_0": "Yasmiṁ samaye sammukhāvinayena ca sativinayena ca adhikaraṇaṁ vūpasammati—",
  "pli-tv-pvr11:37.2_0": "yattha sativinayo tattha sammukhāvinayo,",
  "pli-tv-pvr11:37.2_1": "yattha sammukhāvinayo tattha sativinayo,",
  "pli-tv-pvr11:37.2_2": "na tattha amūḷhavinayo,",
  "pli-tv-pvr11:37.2_3": "na tattha paṭiññātakaraṇaṁ,",
  "pli-tv-pvr11:37.2_4": "na tattha yebhuyyasikā,",
  "pli-tv-pvr11:37.2_5": "na tattha tassapāpiyasikā,",
  "pli-tv-pvr11:37.2_6": "na tattha tiṇavatthārako.",
  "pli-tv-pvr11:37.3_0": "Yasmiṁ samaye sammukhāvinayena ca amūḷhavinayena ca …pe…",
  "pli-tv-pvr11:37.4_0": "sammukhāvinayena ca paṭiññātakaraṇena ca …pe…",
  "pli-tv-pvr11:37.5_0": "sammukhāvinayena ca yebhuyyasikāya ca …pe…",
  "pli-tv-pvr11:37.6_0": "sammukhāvinayena ca tassapāpiyasikāya ca …pe…",
  "pli-tv-pvr11:37.7_0": "sammukhāvinayena ca tiṇavatthārakena ca adhikaraṇaṁ vūpasammati—",
  "pli-tv-pvr11:37.8_0": "yattha tiṇavatthārako tattha sammukhāvinayo,",
  "pli-tv-pvr11:37.8_1": "yattha sammukhāvinayo tattha tiṇavatthārako,",
  "pli-tv-pvr11:37.8_2": "na tattha sativinayo,",
  "pli-tv-pvr11:37.8_3": "na tattha amūḷhavinayo,",
  "pli-tv-pvr11:37.8_4": "na tattha paṭiññātakaraṇaṁ,",
  "pli-tv-pvr11:37.8_5": "na tattha yebhuyyasikā,",
  "pli-tv-pvr11:37.8_6": "na tattha tassapāpiyasikā.",
  "pli-tv-pvr11:38.0_0": "8. Saṁsaṭṭhavāra",
  "pli-tv-pvr11:38.1_0": "Sammukhāvinayoti vā sativinayoti vā ime dhammā saṁsaṭṭhā udāhu visaṁsaṭṭhā?",
  "pli-tv-pvr11:38.2_0": "Labbhā ca panimesaṁ dhammānaṁ vinibbhujitvā vinibbhujitvā nānākaraṇaṁ paññāpetuṁ?",
  "pli-tv-pvr11:38.3_0": "Sammukhāvinayoti vā amūḷhavinayoti vā …pe…",
  "pli-tv-pvr11:38.4_0": "sammukhāvinayoti vā paṭiññātakaraṇanti vā …",
  "pli-tv-pvr11:38.5_0": "sammukhāvinayoti vā yebhuyyasikāti vā …",
  "pli-tv-pvr11:38.6_0": "sammukhāvinayoti vā tassapāpiyasikāti vā …",
  "pli-tv-pvr11:38.7_0": "sammukhāvinayoti vā tiṇavatthārakoti vā—ime dhammā saṁsaṭṭhā, udāhu visaṁsaṭṭhā?",
  "pli-tv-pvr11:38.8_0": "Labbhā ca panimesaṁ dhammānaṁ vinibbhujitvā vinibbhujitvā nānākaraṇaṁ paññāpetuṁ?",
  "pli-tv-pvr11:39.1_0": "Sammukhāvinayoti vā sativinayoti vā—",
  "pli-tv-pvr11:39.2_0": "ime dhammā saṁsaṭṭhā, no visaṁsaṭṭhā;",
  "pli-tv-pvr11:39.3_0": "na ca labbhā imesaṁ dhammānaṁ vinibbhujitvā vinibbhujitvā nānākaraṇaṁ paññāpetuṁ.",
  "pli-tv-pvr11:39.4_0": "Sammukhāvinayoti vā amūḷhavinayoti vā …pe…",
  "pli-tv-pvr11:39.5_0": "sammukhāvinayoti vā paṭiññātakaraṇanti vā …",
  "pli-tv-pvr11:39.6_0": "sammukhāvinayoti vā yebhuyyasikāti vā …",
  "pli-tv-pvr11:39.7_0": "sammukhāvinayoti vā tassapāpiyasikāti vā …",
  "pli-tv-pvr11:39.8_0": "sammukhāvinayoti vā tiṇavatthārakoti vā—",
  "pli-tv-pvr11:39.9_0": "ime dhammā saṁsaṭṭhā, no visaṁsaṭṭhā;",
  "pli-tv-pvr11:39.10_0": "na ca labbhā imesaṁ dhammānaṁ vinibbhujitvā vinibbhujitvā nānākaraṇaṁ paññāpetuṁ.",
  "pli-tv-pvr11:40.0_0": "9. Sattasamathanidāna",
  "pli-tv-pvr11:40.1_0": "Sammukhāvinayo kiṁnidāno, kiṁsamudayo, kiṁjātiko, kiṁpabhavo, kiṁsambhāro kiṁsamuṭṭhāno?",
  "pli-tv-pvr11:40.2_0": "Sativinayo kiṁnidāno, kiṁsamudayo, kiṁjātiko, kiṁpabhavo, kiṁsambhāro, kiṁsamuṭṭhāno?",
  "pli-tv-pvr11:40.3_0": "Amūḷhavinayo kiṁnidāno, kiṁsamudayo, kiṁjātiko, kiṁpabhavo, kiṁsambhāro, kiṁsamuṭṭhāno?",
  "pli-tv-pvr11:40.4_0": "Paṭiññātakaraṇaṁ kiṁnidānaṁ, kiṁsamudayaṁ, kiṁjātikaṁ, kiṁpabhavaṁ, kiṁsambhāraṁ, kiṁsamuṭṭhānaṁ?",
  "pli-tv-pvr11:40.5_0": "Yebhuyyasikā kiṁnidānā, kiṁsamudayā, kiṁjātikā, kiṁpabhavā, kiṁsambhārā, kiṁsamuṭṭhānā?",
  "pli-tv-pvr11:40.6_0": "Tassapāpiyasikā kiṁnidānā, kiṁsamudayā, kiṁjātikā, kiṁpabhavā, kiṁsambhārā, kiṁsamuṭṭhānā?",
  "pli-tv-pvr11:40.7_0": "Tiṇavatthārako kiṁnidāno, kiṁsamudayo, kiṁjātiko, kiṁpabhavo, kiṁsambhāro, kiṁsamuṭṭhāno?",
  "pli-tv-pvr11:41.1_0": "Sammukhāvinayo nidānanidāno,",
  "pli-tv-pvr11:41.1_1": "nidānasamudayo,",
  "pli-tv-pvr11:41.1_2": "nidānajātiko,",
  "pli-tv-pvr11:41.1_3": "nidānapabhavo,",
  "pli-tv-pvr11:41.1_4": "nidānasambhāro,",
  "pli-tv-pvr11:41.1_5": "nidānasamuṭṭhāno.",
  "pli-tv-pvr11:41.2_0": "Sativinayo …pe…",
  "pli-tv-pvr11:41.3_0": "amūḷhavinayo …pe…",
  "pli-tv-pvr11:41.4_0": "paṭiññātakaraṇaṁ nidānanidānaṁ,",
  "pli-tv-pvr11:41.4_1": "nidānasamudayaṁ,",
  "pli-tv-pvr11:41.4_2": "nidānajātikaṁ,",
  "pli-tv-pvr11:41.4_3": "nidānapabhavaṁ,",
  "pli-tv-pvr11:41.4_4": "nidānasambhāraṁ,",
  "pli-tv-pvr11:41.4_5": "nidānasamuṭṭhānaṁ.",
  "pli-tv-pvr11:41.5_0": "Yebhuyyasikā …pe…",
  "pli-tv-pvr11:41.6_0": "tassapāpiyasikā nidānanidānā,",
  "pli-tv-pvr11:41.6_1": "nidānasamudayā,",
  "pli-tv-pvr11:41.6_2": "nidānajātikā,",
  "pli-tv-pvr11:41.6_3": "nidānapabhavā,",
  "pli-tv-pvr11:41.6_4": "nidānasambhārā,",
  "pli-tv-pvr11:41.6_5": "nidānasamuṭṭhānā.",
  "pli-tv-pvr11:41.7_0": "Tiṇavatthārako nidānanidāno,",
  "pli-tv-pvr11:41.7_1": "nidānasamudayo,",
  "pli-tv-pvr11:41.7_2": "nidānajātiko,",
  "pli-tv-pvr11:41.7_3": "nidānapabhavo,",
  "pli-tv-pvr11:41.7_4": "nidānasambhāro,",
  "pli-tv-pvr11:41.7_5": "nidānasamuṭṭhāno.",
  "pli-tv-pvr11:42.1_0": "Sammukhāvinayo kiṁnidāno, kiṁsamudayo, kiṁjātiko, kiṁpabhavo, kiṁsambhāro, kiṁsamuṭṭhāno?",
  "pli-tv-pvr11:42.2_0": "Sativinayo …pe…",
  "pli-tv-pvr11:42.3_0": "amūḷhavinayo …pe…",
  "pli-tv-pvr11:42.4_0": "paṭiññātakaraṇaṁ …pe…",
  "pli-tv-pvr11:42.5_0": "yebhuyyasikā …pe…",
  "pli-tv-pvr11:42.6_0": "tassapāpiyasikā …pe…",
  "pli-tv-pvr11:42.7_0": "tiṇavatthārako kiṁnidāno, kiṁsamudayo, kiṁjātiko, kiṁpabhavo, kiṁsambhāro, kiṁsamuṭṭhāno?",
  "pli-tv-pvr11:43.1_0": "Sammukhāvinayo hetunidāno, hetusamudayo, hetujātiko, hetupabhavo, hetusambhāro, hetusamuṭṭhāno.",
  "pli-tv-pvr11:43.2_0": "Sativinayo …pe…",
  "pli-tv-pvr11:43.3_0": "amūḷhavinayo …pe…",
  "pli-tv-pvr11:43.4_0": "paṭiññātakaraṇaṁ hetunidānaṁ,",
  "pli-tv-pvr11:43.4_1": "hetusamudayaṁ,",
  "pli-tv-pvr11:43.4_2": "hetujātikaṁ,",
  "pli-tv-pvr11:43.4_3": "hetupabhavaṁ,",
  "pli-tv-pvr11:43.4_4": "hetusambhāraṁ,",
  "pli-tv-pvr11:43.4_5": "hetusamuṭṭhānaṁ.",
  "pli-tv-pvr11:43.5_0": "Yebhuyyasikā …pe…",
  "pli-tv-pvr11:43.6_0": "tassapāpiyasikā hetunidānā, hetusamudayā, hetujātikā, hetupabhavā, hetusambhārā, hetusamuṭṭhānā.",
  "pli-tv-pvr11:43.7_0": "Tiṇavatthārako hetunidāno, hetusamudayo, hetujātiko, hetupabhavo, hetusambhāro, hetusamuṭṭhāno.",
  "pli-tv-pvr11:44.1_0": "Sammukhāvinayo kiṁnidāno, kiṁsamudayo, kiṁjātiko, kiṁpabhavo, kiṁsambhāro, kiṁsamuṭṭhāno?",
  "pli-tv-pvr11:44.2_0": "Sativinayo …pe…",
  "pli-tv-pvr11:44.3_0": "amūḷhavinayo …pe…",
  "pli-tv-pvr11:44.4_0": "paṭiññātakaraṇaṁ …pe…",
  "pli-tv-pvr11:44.5_0": "yebhuyyasikā …pe…",
  "pli-tv-pvr11:44.6_0": "tassapāpiyasikā …pe…",
  "pli-tv-pvr11:44.7_0": "tiṇavatthārako kiṁnidāno, kiṁsamudayo, kiṁjātiko, kiṁpabhavo, kiṁsambhāro, kiṁsamuṭṭhāno?",
  "pli-tv-pvr11:44.8_0": "Sammukhāvinayo paccayanidāno,",
  "pli-tv-pvr11:44.8_1": "paccayasamudayo,",
  "pli-tv-pvr11:44.8_2": "paccayajātiko,",
  "pli-tv-pvr11:44.8_3": "paccayapabhavo,",
  "pli-tv-pvr11:44.8_4": "paccayasambhāro,",
  "pli-tv-pvr11:44.8_5": "paccayasamuṭṭhāno.",
  "pli-tv-pvr11:44.9_0": "Sativinayo …pe…",
  "pli-tv-pvr11:44.10_0": "amūḷhavinayo …pe…",
  "pli-tv-pvr11:44.11_0": "paṭiññātakaraṇaṁ paccayanidānaṁ,",
  "pli-tv-pvr11:44.11_1": "paccayasamudayaṁ,",
  "pli-tv-pvr11:44.11_2": "paccayajātikaṁ,",
  "pli-tv-pvr11:44.11_3": "paccayapabhavaṁ,",
  "pli-tv-pvr11:44.11_4": "paccayasambhāraṁ,",
  "pli-tv-pvr11:44.11_5": "paccayasamuṭṭhānaṁ.",
  "pli-tv-pvr11:44.12_0": "Yebhuyyasikā …pe…",
  "pli-tv-pvr11:44.13_0": "tassapāpiyasikā paccayanidānā,",
  "pli-tv-pvr11:44.13_1": "paccayasamudayā,",
  "pli-tv-pvr11:44.13_2": "paccayajātikā,",
  "pli-tv-pvr11:44.13_3": "paccayapabhavā,",
  "pli-tv-pvr11:44.13_4": "paccayasambhārā,",
  "pli-tv-pvr11:44.13_5": "paccayasamuṭṭhānā.",
  "pli-tv-pvr11:44.14_0": "Tiṇavatthārako paccayanidāno,",
  "pli-tv-pvr11:44.14_1": "paccayasamudayo,",
  "pli-tv-pvr11:44.14_2": "paccayajātiko,",
  "pli-tv-pvr11:44.14_3": "paccayapabhavo,",
  "pli-tv-pvr11:44.14_4": "paccayasambhāro,",
  "pli-tv-pvr11:44.14_5": "paccayasamuṭṭhāno.",
  "pli-tv-pvr11:45.1_0": "Sattannaṁ samathānaṁ kati mūlāni, kati samuṭṭhānā?",
  "pli-tv-pvr11:45.2_0": "Sattannaṁ samathānaṁ chabbīsa mūlāni, chattiṁsa samuṭṭhānā.",
  "pli-tv-pvr11:45.3_0": "Sattannaṁ samathānaṁ katamāni chabbīsa mūlāni?",
  "pli-tv-pvr11:45.4_0": "Sammukhāvinayassa cattāri mūlāni—",
  "pli-tv-pvr11:45.5_0": "saṅghasammukhatā, dhammasammukhatā, vinayasammukhatā, puggalasammukhatā;",
  "pli-tv-pvr11:45.6_0": "sativinayassa cattāri mūlāni;",
  "pli-tv-pvr11:45.7_0": "amūḷhavinayassa cattāri mūlāni;",
  "pli-tv-pvr11:45.8_0": "paṭiññātakaraṇassa dve mūlāni—",
  "pli-tv-pvr11:45.9_0": "yo ca deseti yassa ca deseti;",
  "pli-tv-pvr11:45.10_0": "yebhuyyasikāya cattāri mūlāni;",
  "pli-tv-pvr11:45.11_0": "tassapāpiyasikāya cattāri mūlāni;",
  "pli-tv-pvr11:45.12_0": "tiṇavatthārakassa cattāri mūlāni—",
  "pli-tv-pvr11:45.13_0": "saṅghasammukhatā, dhammasammukhatā, vinayasammukhatā, puggalasammukhatā—",
  "pli-tv-pvr11:45.14_0": "sattannaṁ samathānaṁ imāni chabbīsa mūlāni.",
  "pli-tv-pvr11:46.1_0": "Sattannaṁ samathānaṁ katame chattiṁsa samuṭṭhānā?",
  "pli-tv-pvr11:46.2_0": "Sativinayassa kammassa kiriyā, karaṇaṁ, upagamanaṁ, ajjhupagamanaṁ, adhivāsanā, appaṭikkosanā.",
  "pli-tv-pvr11:46.3_0": "Amūḷhavinayassa kammassa …pe…",
  "pli-tv-pvr11:46.4_0": "paṭiññātakaraṇassa kammassa …",
  "pli-tv-pvr11:46.5_0": "yebhuyyasikāya kammassa …",
  "pli-tv-pvr11:46.6_0": "tassapāpiyasikāya kammassa …",
  "pli-tv-pvr11:46.7_0": "tiṇavatthārakassa kammassa kiriyā, karaṇaṁ, upagamanaṁ, ajjhupagamanaṁ, adhivāsanā, appaṭikkosanā—",
  "pli-tv-pvr11:46.8_0": "sattannaṁ samathānaṁ ime chattiṁsa samuṭṭhānā.",
  "pli-tv-pvr11:47.0_0": "10. Sattasamathanānatthādi",
  "pli-tv-pvr11:47.1_0": "Sammukhāvinayoti vā sativinayoti vā—ime dhammā nānatthā nānābyañjanā,",
  "pli-tv-pvr11:47.1_1": "udāhu ekatthā byañjanameva nānaṁ?",
  "pli-tv-pvr11:47.2_0": "Sammukhāvinayoti vā amūḷhavinayoti vā …pe…",
  "pli-tv-pvr11:47.3_0": "sammukhāvinayoti vā paṭiññātakaraṇanti vā …",
  "pli-tv-pvr11:47.4_0": "sammukhāvinayoti vā yebhuyyasikāti vā …",
  "pli-tv-pvr11:47.5_0": "sammukhāvinayoti vā tassapāpiyasikāti vā …",
  "pli-tv-pvr11:47.6_0": "sammukhāvinayoti vā tiṇavatthārakoti vā—ime dhammā nānatthā nānābyañjanā,",
  "pli-tv-pvr11:47.6_1": "udāhu ekatthā byañjanameva nānaṁ?",
  "pli-tv-pvr11:47.7_0": "Sammukhāvinayoti vā sativinayoti vā—ime dhammā nānatthā ceva nānābyañjanā ca.",
  "pli-tv-pvr11:47.8_0": "Sammukhāvinayoti vā amūḷhavinayoti vā …pe…",
  "pli-tv-pvr11:47.9_0": "sammukhāvinayoti vā paṭiññātakaraṇanti vā …",
  "pli-tv-pvr11:47.10_0": "sammukhāvinayoti vā yebhuyyasikāti vā …",
  "pli-tv-pvr11:47.11_0": "sammukhāvinayoti vā tassapāpiyasikāti vā …",
  "pli-tv-pvr11:47.12_0": "sammukhāvinayoti vā tiṇavatthārakoti vā—ime dhammā nānatthā ceva nānābyañjanā ca.",
  "pli-tv-pvr11:48.1_0": "Vivādo vivādādhikaraṇaṁ, vivādo no adhikaraṇaṁ, adhikaraṇaṁ no vivādo, adhikaraṇañceva vivādo ca?",
  "pli-tv-pvr11:48.2_0": "Siyā vivādo vivādādhikaraṇaṁ,",
  "pli-tv-pvr11:48.2_1": "siyā vivādo no adhikaraṇaṁ,",
  "pli-tv-pvr11:48.2_2": "siyā adhikaraṇaṁ no vivādo,",
  "pli-tv-pvr11:48.2_3": "siyā adhikaraṇañceva vivādo ca.",
  "pli-tv-pvr11:49.1_0": "Tattha katamo vivādo vivādādhikaraṇaṁ?",
  "pli-tv-pvr11:49.2_0": "Idha bhikkhū vivadanti dhammoti vā adhammoti vā …pe…",
  "pli-tv-pvr11:49.3_0": "duṭṭhullā āpattīti vā aduṭṭhullā āpattīti vā.",
  "pli-tv-pvr11:49.4_0": "Yaṁ tattha bhaṇḍanaṁ, kalaho, viggaho, vivādo, nānāvādo, aññathāvādo, vipaccatāya vohāro, medhakaṁ—",
  "pli-tv-pvr11:49.5_0": "ayaṁ vivādo vivādādhikaraṇaṁ.",
  "pli-tv-pvr11:50.1_0": "Tattha katamo vivādo no adhikaraṇaṁ?",
  "pli-tv-pvr11:50.2_0": "Mātāpi puttena vivadati,",
  "pli-tv-pvr11:50.2_1": "puttopi mātarā vivadati,",
  "pli-tv-pvr11:50.2_2": "pitāpi puttena vivadati,",
  "pli-tv-pvr11:50.2_3": "puttopi pitarā vivadati,",
  "pli-tv-pvr11:50.2_4": "bhātāpi bhātarā vivadati,",
  "pli-tv-pvr11:50.2_5": "bhātāpi bhaginiyā vivadati,",
  "pli-tv-pvr11:50.2_6": "bhaginīpi bhātarā vivadati,",
  "pli-tv-pvr11:50.2_7": "sahāyopi sahāyena vivadati—",
  "pli-tv-pvr11:50.3_0": "ayaṁ vivādo no adhikaraṇaṁ.",
  "pli-tv-pvr11:51.1_0": "Tattha katamaṁ adhikaraṇaṁ no vivādo?",
  "pli-tv-pvr11:51.2_0": "Anuvādādhikaraṇaṁ, āpattādhikaraṇaṁ, kiccādhikaraṇaṁ—",
  "pli-tv-pvr11:51.3_0": "idaṁ adhikaraṇaṁ no vivādo.",
  "pli-tv-pvr11:52.1_0": "Tattha katamaṁ adhikaraṇañceva vivādo ca?",
  "pli-tv-pvr11:52.2_0": "Vivādādhikaraṇaṁ adhikaraṇañceva vivādo ca.",
  "pli-tv-pvr11:53.1_0": "Anuvādo anuvādādhikaraṇaṁ,",
  "pli-tv-pvr11:53.1_1": "anuvādo no adhikaraṇaṁ,",
  "pli-tv-pvr11:53.1_2": "adhikaraṇaṁ no anuvādo,",
  "pli-tv-pvr11:53.1_3": "adhikaraṇañceva anuvādo ca?",
  "pli-tv-pvr11:53.2_0": "Siyā anuvādo anuvādādhikaraṇaṁ,",
  "pli-tv-pvr11:53.2_1": "siyā anuvādo no adhikaraṇaṁ,",
  "pli-tv-pvr11:53.2_2": "siyā adhikaraṇaṁ no anuvādo,",
  "pli-tv-pvr11:53.2_3": "siyā adhikaraṇañceva anuvādo ca.",
  "pli-tv-pvr11:54.1_0": "Tattha katamo anuvādo anuvādādhikaraṇaṁ?",
  "pli-tv-pvr11:54.2_0": "Idha bhikkhū bhikkhuṁ anuvadanti sīlavipattiyā vā ācāravipattiyā vā diṭṭhivipattiyā vā ājīvavipattiyā vā.",
  "pli-tv-pvr11:54.3_0": "Yo tattha anuvādo, anuvadanā anullapanā anubhaṇanā anusampavaṅkatā abbhussahanatā anubalappadānaṁ—",
  "pli-tv-pvr11:54.4_0": "ayaṁ anuvādo anuvādādhikaraṇaṁ.",
  "pli-tv-pvr11:55.1_0": "Tattha katamo anuvādo no adhikaraṇaṁ?",
  "pli-tv-pvr11:55.2_0": "Mātāpi puttaṁ anuvadati,",
  "pli-tv-pvr11:55.2_1": "puttopi mātaraṁ anuvadati,",
  "pli-tv-pvr11:55.2_2": "pitāpi puttaṁ anuvadati,",
  "pli-tv-pvr11:55.2_3": "puttopi pitaraṁ anuvadati,",
  "pli-tv-pvr11:55.2_4": "bhātāpi bhātaraṁ anuvadati,",
  "pli-tv-pvr11:55.2_5": "bhātāpi bhaginiṁ anuvadati,",
  "pli-tv-pvr11:55.2_6": "bhaginīpi bhātaraṁ anuvadati,",
  "pli-tv-pvr11:55.2_7": "sahāyopi sahāyaṁ anuvadati—",
  "pli-tv-pvr11:55.3_0": "ayaṁ anuvādo no adhikaraṇaṁ.",
  "pli-tv-pvr11:56.1_0": "Tattha katamaṁ adhikaraṇaṁ no anuvādo?",
  "pli-tv-pvr11:56.2_0": "Āpattādhikaraṇaṁ kiccādhikaraṇaṁ vivādādhikaraṇaṁ—",
  "pli-tv-pvr11:56.3_0": "idaṁ adhikaraṇaṁ no anuvādo.",
  "pli-tv-pvr11:57.1_0": "Tattha katamaṁ adhikaraṇañceva anuvādo ca?",
  "pli-tv-pvr11:57.2_0": "Anuvādādhikaraṇaṁ adhikaraṇañceva anuvādo ca.",
  "pli-tv-pvr11:58.1_0": "Āpatti āpattādhikaraṇaṁ, āpatti no adhikaraṇaṁ, adhikaraṇaṁ no āpatti, adhikaraṇañceva āpatti ca?",
  "pli-tv-pvr11:58.2_0": "Siyā āpatti āpattādhikaraṇaṁ,",
  "pli-tv-pvr11:58.2_1": "siyā āpatti no adhikaraṇaṁ,",
  "pli-tv-pvr11:58.2_2": "siyā adhikaraṇaṁ no āpatti,",
  "pli-tv-pvr11:58.2_3": "siyā adhikaraṇañceva āpatti ca.",
  "pli-tv-pvr11:59.1_0": "Tattha katamā āpatti āpattādhikaraṇaṁ?",
  "pli-tv-pvr11:59.2_0": "Pañcapi āpattikkhandhā āpattādhikaraṇaṁ.",
  "pli-tv-pvr11:59.3_0": "Sattapi āpattikkhandhā āpattādhikaraṇaṁ.",
  "pli-tv-pvr11:59.4_0": "Ayaṁ āpatti āpattādhikaraṇaṁ.",
  "pli-tv-pvr11:60.1_0": "Tattha katamā āpatti no adhikaraṇaṁ?",
  "pli-tv-pvr11:60.2_0": "Sotāpatti samāpatti—",
  "pli-tv-pvr11:60.3_0": "ayaṁ āpatti no adhikaraṇaṁ.",
  "pli-tv-pvr11:61.1_0": "Tattha katamaṁ adhikaraṇaṁ no āpatti?",
  "pli-tv-pvr11:61.2_0": "Kiccādhikaraṇaṁ vivādādhikaraṇaṁ anuvādādhikaraṇaṁ—",
  "pli-tv-pvr11:61.3_0": "idaṁ adhikaraṇaṁ no āpatti.",
  "pli-tv-pvr11:62.1_0": "Tattha katamaṁ adhikaraṇañceva āpatti ca?",
  "pli-tv-pvr11:62.2_0": "Āpattādhikaraṇaṁ adhikaraṇañceva āpatti ca.",
  "pli-tv-pvr11:63.1_0": "Kiccaṁ kiccādhikaraṇaṁ, kiccaṁ no adhikaraṇaṁ, adhikaraṇaṁ no kiccaṁ, adhikaraṇañceva kiccañca?",
  "pli-tv-pvr11:63.2_0": "Siyā kiccaṁ kiccādhikaraṇaṁ,",
  "pli-tv-pvr11:63.2_1": "siyā kiccaṁ no adhikaraṇaṁ,",
  "pli-tv-pvr11:63.2_2": "siyā adhikaraṇaṁ no kiccaṁ,",
  "pli-tv-pvr11:63.2_3": "siyā adhikaraṇañceva kiccañca.",
  "pli-tv-pvr11:64.1_0": "Tattha katamaṁ kiccaṁ kiccādhikaraṇaṁ?",
  "pli-tv-pvr11:64.2_0": "Yā saṅghassa kiccayatā karaṇīyatā apalokanakammaṁ ñattikammaṁ ñattidutiyakammaṁ ñatticatutthakammaṁ—",
  "pli-tv-pvr11:64.3_0": "idaṁ kiccaṁ kiccādhikaraṇaṁ.",
  "pli-tv-pvr11:65.1_0": "Tattha katamaṁ kiccaṁ no adhikaraṇaṁ?",
  "pli-tv-pvr11:65.2_0": "Ācariyakiccaṁ upajjhāyakiccaṁ samānupajjhāyakiccaṁ samānācariyakiccaṁ—",
  "pli-tv-pvr11:65.3_0": "idaṁ kiccaṁ no adhikaraṇaṁ.",
  "pli-tv-pvr11:66.1_0": "Tattha katamaṁ adhikaraṇaṁ no kiccaṁ?",
  "pli-tv-pvr11:66.2_0": "Vivādādhikaraṇaṁ anuvādādhikaraṇaṁ āpattādhikaraṇaṁ—",
  "pli-tv-pvr11:66.3_0": "idaṁ adhikaraṇaṁ no kiccaṁ.",
  "pli-tv-pvr11:67.1_0": "Tattha katamaṁ adhikaraṇañceva kiccañca?",
  "pli-tv-pvr11:67.2_0": "Kiccādhikaraṇaṁ adhikaraṇañceva kiccaṁ cāti.",
  "pli-tv-pvr11:68.1_0": "Adhikaraṇabhedaṁ niṭṭhitaṁ.",
  "pli-tv-pvr11:69.1_0": "Tassuddānaṁ",
  "pli-tv-pvr11:70.1_0": "Adhikaraṇaṁ ukkoṭā,",
  "pli-tv-pvr11:70.2_0": "ākārā puggalena ca;",
  "pli-tv-pvr11:70.3_0": "Nidānahetupaccayā,",
  "pli-tv-pvr11:70.4_0": "mūlaṁ samuṭṭhānena ca.",
  "pli-tv-pvr11:71.1_0": "Āpatti hoti yattha ca,",
  "pli-tv-pvr11:71.2_0": "saṁsaṭṭhā nidānena ca;",
  "pli-tv-pvr11:71.3_0": "Hetupaccayamūlāni,",
  "pli-tv-pvr11:71.4_0": "samuṭṭhānena byañjanā;",
  "pli-tv-pvr11:71.5_0": "Vivādo adhikaraṇanti,",
  "pli-tv-pvr11:71.6_0": "bhedādhikaraṇe idanti."
}