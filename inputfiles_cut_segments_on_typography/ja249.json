{
  "ja249:0.1_0": "Jātaka",
  "ja249:0.2_0": "Dukanipāta",
  "ja249:0.3_0": "Siṅgālavagga",
  "ja249:0.4_0": "9. Sālakajātaka",
  "ja249:1.1_0": "“Ekaputtako bhavissasi,",
  "ja249:1.2_0": "Tvañca no hessasi issaro kule;",
  "ja249:1.3_0": "Oroha dumasmā sālaka,",
  "ja249:1.4_0": "Ehi dāni gharakaṁ vajemase”.",
  "ja249:2.1_0": "“Nanu maṁ suhadayoti maññasi,",
  "ja249:2.2_0": "Yañca maṁ hanasi veḷuyaṭṭhiyā;",
  "ja249:2.3_0": "Pakkambavane ramāmase,",
  "ja249:2.4_0": "Gaccha tvaṁ gharakaṁ yathāsukhan”ti.",
  "ja249:3.1_0": "Sālakajātakaṁ navamaṁ."
}