{
  "kv9.10:0.1_0": "Kathāvatthu",
  "kv9.10:0.2_0": "Mahāpaṇṇāsaka",
  "kv9.10:0.3_0": "Navamavagga",
  "kv9.10:0.4_0": "Nayathācittassakāyakammantikathā",
  "kv9.10:1.1_0": "Na yathācittassa kāyakammanti?",
  "kv9.10:1.2_0": "Āmantā.",
  "kv9.10:1.3_0": "Aphassakassa kāyakammaṁ …pe…",
  "kv9.10:1.4_0": "acittakassa kāyakammanti?",
  "kv9.10:1.5_0": "Na hevaṁ vattabbe …pe…",
  "kv9.10:1.6_0": "nanu saphassakassa kāyakammaṁ …pe…",
  "kv9.10:1.7_0": "sacittakassa kāyakammanti?",
  "kv9.10:1.8_0": "Āmantā.",
  "kv9.10:1.9_0": "Hañci saphassakassa kāyakammaṁ …pe…",
  "kv9.10:1.10_0": "sacittakassa kāyakammaṁ, no ca vata re vattabbe—",
  "kv9.10:1.11_0": "“na yathācittassa kāyakamman”ti.",
  "kv9.10:2.1_0": "Na yathācittassa kāyakammanti?",
  "kv9.10:2.2_0": "Āmantā.",
  "kv9.10:2.3_0": "Anāvaṭṭentassa kāyakammaṁ …pe… appaṇidahantassa kāyakammanti?",
  "kv9.10:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv9.10:2.5_0": "nanu āvaṭṭentassa kāyakammaṁ …pe… paṇidahantassa kāyakammanti?",
  "kv9.10:2.6_0": "Āmantā.",
  "kv9.10:2.7_0": "Hañci āvaṭṭentassa kāyakammaṁ …pe… paṇidahantassa kāyakammaṁ, no ca vata re vattabbe—",
  "kv9.10:2.8_0": "“na yathācittassa kāyakamman”ti.",
  "kv9.10:3.1_0": "Na yathācittassa kāyakammanti?",
  "kv9.10:3.2_0": "Āmantā.",
  "kv9.10:3.3_0": "Nanu kāyakammaṁ cittasamuṭṭhānaṁ cittena sahajātaṁ cittena saha ekuppādanti?",
  "kv9.10:3.4_0": "Āmantā.",
  "kv9.10:3.5_0": "Hañci kāyakammaṁ cittasamuṭṭhānaṁ cittena sahajātaṁ cittena saha ekuppādaṁ, no ca vata re vattabbe—",
  "kv9.10:3.6_0": "“na yathācittassa kāyakamman”ti.",
  "kv9.10:4.1_0": "Na yathācittassa kāyakammanti?",
  "kv9.10:4.2_0": "Āmantā.",
  "kv9.10:4.3_0": "Na abhikkamitukāmo abhikkamati,",
  "kv9.10:4.3_1": "na paṭikkamitukāmo paṭikkamati,",
  "kv9.10:4.3_2": "na āloketukāmo āloketi,",
  "kv9.10:4.3_3": "na viloketukāmo viloketi,",
  "kv9.10:4.3_4": "na samiñjitukāmo samiñjeti,",
  "kv9.10:4.3_5": "na pasāretukāmo pasāretīti?",
  "kv9.10:4.4_0": "Na hevaṁ vattabbe …pe….",
  "kv9.10:4.5_0": "Nanu abhikkamitukāmo abhikkamati,",
  "kv9.10:4.5_1": "paṭikkamitukāmo paṭikkamati,",
  "kv9.10:4.5_2": "āloketukāmo āloketi,",
  "kv9.10:4.5_3": "viloketukāmo viloketi,",
  "kv9.10:4.5_4": "samiñjitukāmo samiñjeti,",
  "kv9.10:4.5_5": "pasāretukāmo pasāretīti?",
  "kv9.10:4.6_0": "Āmantā.",
  "kv9.10:4.7_0": "Hañci abhikkamitukāmo abhikkamati …pe…",
  "kv9.10:4.8_0": "pasāretukāmo pasāreti, no ca vata re vattabbe—",
  "kv9.10:4.9_0": "“na yathācittassa kāyakamman”ti.",
  "kv9.10:5.1_0": "Na vattabbaṁ—",
  "kv9.10:5.2_0": "“na yathācittassa kāyakamman”ti?",
  "kv9.10:5.3_0": "Āmantā.",
  "kv9.10:5.4_0": "Nanu atthi koci “aññatra gacchissāmī”ti aññatra gacchati …pe…",
  "kv9.10:5.5_0": "“aññaṁ pasāressāmī”ti aññaṁ pasāretīti?",
  "kv9.10:5.6_0": "Āmantā.",
  "kv9.10:5.7_0": "Hañci atthi koci “aññatra gacchissāmī”ti aññatra gacchati …pe…",
  "kv9.10:5.8_0": "“aññaṁ pasāressāmī”ti aññaṁ pasāreti, tena vata re vattabbe—",
  "kv9.10:5.9_0": "“na yathācittassa kāyakamman”ti.",
  "kv9.10:5.10_0": "Na yathācittassa kāyakammantikathā niṭṭhitā."
}