{
  "tha-ap294:0.1_0": "Therāpadāna",
  "tha-ap294:0.2_0": "Citakapūjakavagga",
  "tha-ap294:0.3_0": "2. Pupphadhārakattheraapadāna",
  "tha-ap294:1.1_0": "“Vākacīradharo āsiṁ,",
  "tha-ap294:1.2_0": "ajinuttaravāsano;",
  "tha-ap294:1.3_0": "Abhiññā pañca nibbattā,",
  "tha-ap294:1.4_0": "candassa parimajjako.",
  "tha-ap294:2.1_0": "Vipassiṁ lokapajjotaṁ,",
  "tha-ap294:2.2_0": "disvā abhigataṁ mamaṁ;",
  "tha-ap294:2.3_0": "Pāricchattakapupphāni,",
  "tha-ap294:2.4_0": "dhāresiṁ satthuno ahaṁ.",
  "tha-ap294:3.1_0": "Ekanavutito kappe,",
  "tha-ap294:3.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap294:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap294:3.4_0": "dhāraṇāya idaṁ phalaṁ.",
  "tha-ap294:4.1_0": "Sattāsītimhito kappe,",
  "tha-ap294:4.2_0": "eko āsiṁ mahīpati;",
  "tha-ap294:4.3_0": "Samantadhāraṇo nāma,",
  "tha-ap294:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap294:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap294:5.2_0": "…pe…",
  "tha-ap294:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap294:6.1_0": "Itthaṁ sudaṁ āyasmā pupphadhārako thero imā gāthāyo abhāsitthāti.",
  "tha-ap294:7.1_0": "Pupphadhārakattherassāpadānaṁ dutiyaṁ."
}