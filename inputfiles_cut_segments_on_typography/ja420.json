{
  "ja420:0.1_0": "Jātaka",
  "ja420:0.2_0": "Aṭṭhakanipāta",
  "ja420:0.3_0": "Kaccānivagga",
  "ja420:0.4_0": "4. Sumaṅgalajātaka",
  "ja420:1.1_0": "“Bhusamhi kuddhoti avekkhiyāna,",
  "ja420:1.2_0": "Na tāva daṇḍaṁ paṇayeyya issaro;",
  "ja420:1.3_0": "Aṭṭhānaso appatirūpamattano,",
  "ja420:1.4_0": "Parassa dukkhāni bhusaṁ udīraye.",
  "ja420:2.1_0": "Yato ca jāneyya pasādamattano,",
  "ja420:2.2_0": "Atthaṁ niyuñjeyya parassa dukkaṭaṁ;",
  "ja420:2.3_0": "Tadāyamatthoti sayaṁ avekkhiya,",
  "ja420:2.4_0": "Athassa daṇḍaṁ sadisaṁ nivesaye.",
  "ja420:3.1_0": "Na cāpi jhāpeti paraṁ na attanaṁ,",
  "ja420:3.2_0": "Amucchito yo nayate nayānayaṁ;",
  "ja420:3.3_0": "Yo daṇḍadhāro bhavatīdha issaro,",
  "ja420:3.4_0": "Sa vaṇṇagutto siriyā na dhaṁsati.",
  "ja420:4.1_0": "Ye khattiyā se anisammakārino,",
  "ja420:4.2_0": "Paṇenti daṇḍaṁ sahasā pamucchitā;",
  "ja420:4.3_0": "Avaṇṇasaṁyutā jahanti jīvitaṁ,",
  "ja420:4.4_0": "Ito vimuttāpi ca yanti duggatiṁ.",
  "ja420:5.1_0": "Dhamme ca ye ariyappavedite ratā,",
  "ja420:5.2_0": "Anuttarā te vacasā manasā kammunā ca;",
  "ja420:5.3_0": "Te santisoraccasamādhisaṇṭhitā,",
  "ja420:5.4_0": "Vajanti lokaṁ dubhayaṁ tathāvidhā.",
  "ja420:6.1_0": "Rājāhamasmi narapamadānamissaro,",
  "ja420:6.2_0": "Sacepi kujjhāmi ṭhapemi attanaṁ;",
  "ja420:6.3_0": "Nisedhayanto janataṁ tathāvidhaṁ,",
  "ja420:6.4_0": "Paṇemi daṇḍaṁ anukampa yoniso”.",
  "ja420:7.1_0": "“Sirī ca lakkhī ca taveva khattiya,",
  "ja420:7.2_0": "Janādhipa mā vijahi kudācanaṁ;",
  "ja420:7.3_0": "Akkodhano niccapasannacitto,",
  "ja420:7.4_0": "Anīgho tuvaṁ vassasatāni pālaya.",
  "ja420:8.1_0": "Guṇehi etehi upeta khattiya,",
  "ja420:8.2_0": "Ṭhitamariyavattī suvaco akodhano;",
  "ja420:8.3_0": "Sukhī anuppīḷa pasāsamediniṁ,",
  "ja420:8.4_0": "Ito vimuttopi ca yāhi suggatiṁ.",
  "ja420:9.1_0": "Evaṁ sunītena subhāsitena,",
  "ja420:9.2_0": "Dhammena ñāyena upāyaso nayaṁ;",
  "ja420:9.3_0": "Nibbāpaye saṅkhubhitaṁ mahājanaṁ,",
  "ja420:9.4_0": "Mahāva megho salilena medinin”ti.",
  "ja420:10.1_0": "Sumaṅgalajātakaṁ catutthaṁ."
}