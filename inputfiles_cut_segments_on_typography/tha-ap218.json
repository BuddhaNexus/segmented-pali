{
  "tha-ap218:0.1_0": "Therāpadāna",
  "tha-ap218:0.2_0": "Hatthivagga",
  "tha-ap218:0.3_0": "6 Sandhitattheraapadāna",
  "tha-ap218:1.1_0": "“Assatthe haritobhāse,",
  "tha-ap218:1.2_0": "saṁvirūḷhamhi pādape;",
  "tha-ap218:1.3_0": "Ekaṁ buddhagataṁ saññaṁ,",
  "tha-ap218:1.4_0": "alabhiṁhaṁ patissato.",
  "tha-ap218:2.1_0": "Ekattiṁse ito kappe,",
  "tha-ap218:2.2_0": "yaṁ saññamalabhiṁ tadā;",
  "tha-ap218:2.3_0": "Tassā saññāya vāhasā,",
  "tha-ap218:2.4_0": "patto me āsavakkhayo.",
  "tha-ap218:3.1_0": "Ito terasakappamhi,",
  "tha-ap218:3.2_0": "dhaniṭṭho nāma khattiyo;",
  "tha-ap218:3.3_0": "Sattaratanasampanno,",
  "tha-ap218:3.4_0": "cakkavattī mahabbalo.",
  "tha-ap218:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap218:4.2_0": "…pe…",
  "tha-ap218:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap218:5.1_0": "Itthaṁ sudaṁ āyasmā sandhito thero imā gāthāyo abhāsitthāti.",
  "tha-ap218:6.1_0": "Sandhitattherassāpadānaṁ chaṭṭhaṁ."
}