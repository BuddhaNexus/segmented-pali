  "kv2.9:3.6_0": "Āmantā.",
  "kv2.9:3.7_0": "Anupubbena anāgāmiphalaṁ sacchikarotīti?",
  "kv2.9:3.8_0": "Na hevaṁ vattabbe.",
  "kv2.9:4.1_0": "Anupubbābhisamayoti?",
  "kv2.9:4.2_0": "Āmantā.",
  "kv2.9:4.3_0": "Anupubbena arahattamaggaṁ bhāvetīti?",
  "kv2.9:4.4_0": "Na hevaṁ vattabbe.",
  "kv2.9:4.5_0": "Anupubbena arahattamaggaṁ bhāvetīti?",
  "kv2.9:4.6_0": "Āmantā.",
  "kv2.9:4.7_0": "Anupubbena arahattaphalaṁ sacchikarotīti?",
  "kv2.9:4.8_0": "Na hevaṁ vattabbe.",
  "kv2.9:5.1_0": "Sotāpattiphalasacchikiriyāya paṭipanno puggalo dukkhadassanena kiṁ jahatīti?",
  "kv2.9:5.2_0": "Sakkāyadiṭṭhiṁ, vicikicchaṁ, sīlabbataparāmāsaṁ, tadekaṭṭhe ca kilese catubhāgaṁ jahatīti.",
  "kv2.9:5.3_0": "Catubhāgaṁ sotāpanno,",
  "kv2.9:5.3_1": "catubhāgaṁ na sotāpanno,",
  "kv2.9:5.3_2": "catubhāgaṁ sotāpattiphalappatto paṭiladdho adhigato sacchikato upasampajja viharati,",
  "kv2.9:5.3_3": "kāyena phusitvā viharati,",
  "kv2.9:5.3_4": "catubhāgaṁ na kāyena phusitvā viharati,",
  "kv2.9:5.3_5": "catubhāgaṁ sattakkhattuparamo kolaṅkolo ekabījī buddhe aveccappasādena samannāgato,",
  "kv2.9:5.3_6": "dhamme …pe…",
  "kv2.9:5.3_7": "saṅghe …pe…",
  "kv2.9:5.3_8": "ariyakantehi sīlehi samannāgato catubhāgaṁ na ariyakantehi sīlehi samannāgatoti?",
  "kv2.9:5.4_0": "Na hevaṁ vattabbe.",
  "kv2.9:6.1_0": "Samudayadassanena …pe…",
  "kv2.9:6.2_0": "nirodhadassanena …pe…",
  "kv2.9:6.3_0": "maggadassanena kiṁ jahatīti?",
  "kv2.9:6.4_0": "Sakkāyadiṭṭhiṁ, vicikicchaṁ, sīlabbataparāmāsaṁ, tadekaṭṭhe ca kilese catubhāgaṁ jahatīti.",
  "kv2.9:6.5_0": "Catubhāgaṁ sotāpanno,",
  "kv2.9:6.5_1": "catubhāgaṁ na sotāpanno,",
  "kv2.9:6.5_2": "catubhāgaṁ sotāpattiphalappatto paṭiladdho adhigato sacchikato upasampajja viharati,",
  "kv2.9:6.5_3": "kāyena phusitvā viharati,",
  "kv2.9:6.5_4": "catubhāgaṁ na kāyena phusitvā viharati,",
  "kv2.9:6.5_5": "catubhāgaṁ sattakkhattuparamo kolaṅkolo ekabījī buddhe aveccappasādena samannāgato,",
  "kv2.9:6.5_6": "dhamme …pe…",
  "kv2.9:6.5_7": "saṅghe …pe…",
  "kv2.9:6.5_8": "ariyakantehi sīlehi samannāgato,",
  "kv2.9:6.5_9": "catubhāgaṁ na ariyakantehi sīlehi samannāgatoti?",
  "kv2.9:6.6_0": "Na hevaṁ vattabbe …pe….",
  "kv2.9:7.1_0": "Sakadāgāmiphalasacchikiriyāya paṭipanno puggalo dukkhadassanena kiṁ jahatīti?",
  "kv2.9:7.2_0": "Oḷārikaṁ kāmarāgaṁ, oḷārikaṁ byāpādaṁ, tadekaṭṭhe ca kilese catubhāgaṁ jahatīti.",
  "kv2.9:7.3_0": "Catubhāgaṁ sakadāgāmī,",
  "kv2.9:7.3_1": "catubhāgaṁ na sakadāgāmī,",
  "kv2.9:7.3_2": "catubhāgaṁ sakadāgāmiphalappatto paṭiladdho adhigato sacchikato upasampajja viharati,",
  "kv2.9:7.3_3": "kāyena phusitvā viharati,",
  "kv2.9:7.3_4": "catubhāgaṁ na kāyena phusitvā viharatīti?",
  "kv2.9:7.4_0": "Na hevaṁ vattabbe …pe…",
  "kv2.9:7.5_0": "samudayadassanena …pe…",
  "kv2.9:7.6_0": "nirodhadassanena …pe…",
  "kv2.9:7.7_0": "maggadassanena kiṁ jahatīti?",
  "kv2.9:7.8_0": "Oḷārikaṁ kāmarāgaṁ, oḷārikaṁ byāpādaṁ, tadekaṭṭhe ca kilese catubhāgaṁ jahatīti.",
  "kv2.9:7.9_0": "Catubhāgaṁ sakadāgāmī,",
  "kv2.9:7.9_1": "catubhāgaṁ na sakadāgāmī,",
  "kv2.9:7.9_2": "catubhāgaṁ sakadāgāmiphalappatto paṭiladdho adhigato sacchikato upasampajja viharati,",
  "kv2.9:7.9_3": "kāyena phusitvā viharati,",
  "kv2.9:7.9_4": "catubhāgaṁ na kāyena phusitvā viharatīti?",
  "kv2.9:7.10_0": "Na hevaṁ vattabbe …pe….",
  "kv2.9:8.1_0": "Anāgāmiphalasacchikiriyāya paṭipanno puggalo dukkhadassanena kiṁ jahatīti?",
  "kv2.9:8.2_0": "Aṇusahagataṁ kāmarāgaṁ, aṇusahagataṁ byāpādaṁ, tadekaṭṭhe ca kilese catubhāgaṁ jahatīti.",
  "kv2.9:8.3_0": "Catubhāgaṁ anāgāmī,",
  "kv2.9:8.3_1": "catubhāgaṁ na anāgāmī,",
  "kv2.9:8.3_2": "catubhāgaṁ anāgāmiphalappatto paṭiladdho adhigato sacchikato upasampajja viharati,",
  "kv2.9:8.3_3": "kāyena phusitvā viharati,",
  "kv2.9:8.3_4": "catubhāgaṁ na kāyena phusitvā viharati,",
  "kv2.9:8.3_5": "catubhāgaṁ antarāparinibbāyī …pe…",
  "kv2.9:8.3_6": "upahaccaparinibbāyī …",
  "kv2.9:8.3_7": "asaṅkhāraparinibbāyī …",
  "kv2.9:8.3_8": "sasaṅkhāraparinibbāyī …",
  "kv2.9:8.3_9": "uddhaṁsoto akaniṭṭhagāmī,",
  "kv2.9:8.3_10": "catubhāgaṁ na uddhaṁsoto na akaniṭṭhagāmīti?",
  "kv2.9:8.4_0": "Na hevaṁ vattabbe …pe….",
  "kv2.9:9.1_0": "Samudayadassanena …pe…",
  "kv2.9:9.2_0": "nirodhadassanena …pe…",
  "kv2.9:9.3_0": "maggadassanena kiṁ jahatīti?",
  "kv2.9:9.4_0": "Aṇusahagataṁ kāmarāgaṁ, aṇusahagataṁ byāpādaṁ, tadekaṭṭhe ca kilese catubhāgaṁ jahatīti.",
  "kv2.9:9.5_0": "Catubhāgaṁ anāgāmī,",
  "kv2.9:9.5_1": "catubhāgaṁ na anāgāmī,",
  "kv2.9:9.5_2": "catubhāgaṁ anāgāmiphalappatto paṭiladdho adhigato sacchikato upasampajja viharati,",
  "kv2.9:9.5_3": "kāyena phusitvā viharati,",
  "kv2.9:9.5_4": "catubhāgaṁ na kāyena phusitvā viharati,",
  "kv2.9:9.5_5": "catubhāgaṁ antarāparinibbāyī …pe…",
  "kv2.9:9.5_6": "upahaccaparinibbāyī …",
  "kv2.9:9.5_7": "asaṅkhāraparinibbāyī …",
  "kv2.9:9.5_8": "sasaṅkhāraparinibbāyī …",
  "kv2.9:9.5_9": "uddhaṁsoto akaniṭṭhagāmī,",
  "kv2.9:9.5_10": "catubhāgaṁ na uddhaṁsoto akaniṭṭhagāmīti?",
  "kv2.9:9.6_0": "Na hevaṁ vattabbe …pe….",
  "kv2.9:10.1_0": "Arahattasacchikiriyāya paṭipanno puggalo dukkhadassanena kiṁ jahatīti?",
  "kv2.9:10.2_0": "Rūparāgaṁ, arūparāgaṁ, mānaṁ, uddhaccaṁ, avijjaṁ, tadekaṭṭhe ca kilese catubhāgaṁ jahatīti.",
  "kv2.9:10.3_0": "Catubhāgaṁ arahā,",
  "kv2.9:10.3_1": "catubhāgaṁ na arahā,",
  "kv2.9:10.3_2": "catubhāgaṁ arahattappatto paṭiladdho adhigato sacchikato upasampajja viharati,",
  "kv2.9:10.3_3": "kāyena phusitvā viharati,",
  "kv2.9:10.3_4": "catubhāgaṁ na kāyena phusitvā viharati,",
  "kv2.9:10.3_5": "catubhāgaṁ vītarāgo …pe…",
  "kv2.9:10.3_6": "vītadoso …",
  "kv2.9:10.3_7": "vītamoho …pe…",
  "kv2.9:10.3_8": "katakaraṇīyo ohitabhāro anuppattasadattho parikkhīṇabhavasaṁyojano sammadaññāvimutto ukkhittapaligho",
  "kv2.9:10.3_9": "saṅkiṇṇaparikho abbūḷhesiko niraggaḷo ariyo pannaddhajo pannabhāro visaññutto suvijitavijayo,",
  "kv2.9:10.3_10": "dukkhaṁ tassa pariññātaṁ,",
  "kv2.9:10.3_11": "samudayo pahīno,",
  "kv2.9:10.3_12": "nirodho sacchikato,",
  "kv2.9:10.3_13": "maggo bhāvito,",
  "kv2.9:10.3_14": "abhiññeyyaṁ abhiññātaṁ,",
  "kv2.9:10.3_15": "pariññeyyaṁ pariññātaṁ,",
  "kv2.9:10.3_16": "pahātabbaṁ pahīnaṁ,",
  "kv2.9:10.3_17": "bhāvetabbaṁ bhāvitaṁ …pe…",
  "kv2.9:10.3_18": "sacchikātabbaṁ sacchikataṁ,",
  "kv2.9:10.3_19": "catubhāgaṁ sacchikātabbaṁ na sacchikatanti?",
  "kv2.9:10.4_0": "Na hevaṁ vattabbe …pe….",
  "kv2.9:11.1_0": "Samudayadassanena … nirodhadassanena … maggadassanena kiṁ jahatīti?",
  "kv2.9:11.2_0": "Rūparāgaṁ, arūparāgaṁ, mānaṁ, uddhaccaṁ, avijjaṁ, tadekaṭṭhe ca kilese catubhāgaṁ jahatīti.",
  "kv2.9:11.3_0": "Catubhāgaṁ arahā,",
  "kv2.9:11.3_1": "catubhāgaṁ na arahā,",
  "kv2.9:11.3_2": "catubhāgaṁ arahattappatto paṭiladdho adhigato sacchikato upasampajja viharati,",
  "kv2.9:11.3_3": "kāyena phusitvā viharati,",
  "kv2.9:11.3_4": "catubhāgaṁ na kāyena phusitvā viharati,",
  "kv2.9:11.3_5": "catubhāgaṁ vītarāgo …pe…",
  "kv2.9:11.3_6": "vītadoso …",
  "kv2.9:11.3_7": "vītamoho …",
  "kv2.9:11.3_8": "katakaraṇīyo ohitabhāro anuppattasadattho parikkhīṇabhavasaṁyojano sammadaññāvimutto ukkhittapaligho",
  "kv2.9:11.3_9": "saṅkiṇṇaparikho abbūḷhesiko niraggaḷo ariyo pannaddhajo pannabhāro visaññutto suvijitavijayo,",
  "kv2.9:11.3_10": "dukkhaṁ tassa pariññātaṁ,",
  "kv2.9:11.3_11": "samudayo pahīno,",
  "kv2.9:11.3_12": "nirodho sacchikato,",
  "kv2.9:11.3_13": "maggo bhāvito,",
  "kv2.9:11.3_14": "abhiññeyyaṁ abhiññātaṁ,",
  "kv2.9:11.3_15": "pariññeyyaṁ pariññātaṁ,",
  "kv2.9:11.3_16": "pahātabbaṁ pahīnaṁ,",
  "kv2.9:11.3_17": "bhāvitabbaṁ bhāvitaṁ …pe…",
  "kv2.9:11.3_18": "sacchikātabbaṁ sacchikataṁ,",
  "kv2.9:11.3_19": "catubhāgaṁ sacchikātabbaṁ na sacchikatanti?",
  "kv2.9:11.4_0": "Na hevaṁ vattabbe …pe….",
  "kv2.9:12.1_0": "Sotāpattiphalasacchikiriyāya paṭipanno puggalo dukkhaṁ dakkhanto paṭipannakoti vattabboti?",
  "kv2.9:12.2_0": "Āmantā.",
  "kv2.9:12.3_0": "Dukkhe diṭṭhe phale ṭhitoti vattabboti?",
  "kv2.9:12.4_0": "Na hevaṁ vattabbe.",
  "kv2.9:12.5_0": "Samudayaṁ dakkhanto …pe…",
  "kv2.9:12.6_0": "nirodhaṁ dakkhanto paṭipannakoti vattabboti?",
  "kv2.9:12.7_0": "Āmantā.",
  "kv2.9:12.8_0": "Nirodhe diṭṭhe phale ṭhitoti vattabboti?",
  "kv2.9:12.9_0": "Na hevaṁ vattabbe.",
  "kv2.9:13.1_0": "Sotāpattiphalasacchikiriyāya paṭipanno puggalo maggaṁ dakkhanto paṭipannakoti vattabbo,",
  "kv2.9:13.1_1": "magge diṭṭhe phale ṭhitoti vattabboti?",
  "kv2.9:13.2_0": "Āmantā.",
  "kv2.9:13.3_0": "Dukkhaṁ dakkhanto paṭipannakoti vattabbo, dukkhe diṭṭhe phale ṭhitoti vattabboti?",
  "kv2.9:13.4_0": "Na hevaṁ vattabbe …pe…",
  "kv2.9:13.5_0": "maggaṁ dakkhanto paṭipannakoti vattabbo, magge diṭṭhe phale ṭhitoti vattabboti?",
  "kv2.9:13.6_0": "Āmantā.",
  "kv2.9:13.7_0": "Samudayaṁ dakkhanto …pe…",
  "kv2.9:13.8_0": "nirodhaṁ dakkhanto paṭipannakoti vattabbo, nirodhe diṭṭhe phale ṭhitoti vattabboti?",
  "kv2.9:13.9_0": "Na hevaṁ vattabbe …pe….",
  "kv2.9:14.1_0": "Sotāpattiphalasacchikiriyāya paṭipanno puggalo dukkhaṁ dakkhanto paṭipannakoti vattabbo,",
  "kv2.9:14.1_1": "dukkhe diṭṭhe na vattabbaṁ—",
  "kv2.9:14.2_0": "“phale ṭhitoti vattabbo”ti?",
  "kv2.9:14.3_0": "Āmantā.",
  "kv2.9:14.4_0": "Maggaṁ dakkhanto paṭipannakoti vattabbo, magge diṭṭhe na vattabbaṁ—",
  "kv2.9:14.5_0": "“phale ṭhitoti vattabbo”ti?",
  "kv2.9:14.6_0": "Na hevaṁ vattabbe …pe…",
  "kv2.9:14.7_0": "samudayaṁ dakkhanto …",
  "kv2.9:14.8_0": "nirodhaṁ dakkhanto paṭipannakoti vattabbo, nirodhe diṭṭhe na vattabbaṁ—",
  "kv2.9:14.9_0": "“phale ṭhitoti vattabbo”ti?",
  "kv2.9:14.10_0": "Āmantā.",
  "kv2.9:14.11_0": "Maggaṁ dakkhanto “paṭipannako”ti vattabbo, magge diṭṭhe na vattabbaṁ—",
  "kv2.9:14.12_0": "“phale ṭhitoti vattabbo”ti?",
  "kv2.9:14.13_0": "Na hevaṁ vattabbe …pe….",
  "kv2.9:15.1_0": "Sotāpattiphalasacchikiriyāya paṭipanno puggalo dukkhaṁ dakkhanto paṭipannakoti vattabbo,",
  "kv2.9:15.1_1": "dukkhe diṭṭhe na vattabbaṁ—",
  "kv2.9:15.2_0": "“phale ṭhitoti vattabbo”ti?",
  "kv2.9:15.3_0": "Āmantā.",
  "kv2.9:15.4_0": "Niratthiyaṁ dukkhadassananti?",
  "kv2.9:15.5_0": "Na hevaṁ vattabbe …pe…",
  "kv2.9:15.6_0": "samudayaṁ dakkhanto …pe…",
  "kv2.9:15.7_0": "nirodhaṁ dakkhanto paṭipannakoti vattabbo, nirodhe diṭṭhe na vattabbaṁ—",
  "kv2.9:15.8_0": "“phale ṭhitoti vattabbo”ti?",
  "kv2.9:15.9_0": "Āmantā.",
  "kv2.9:15.10_0": "Niratthiyaṁ nirodhadassananti?",
  "kv2.9:15.11_0": "Na hevaṁ vattabbe …pe….",
  "kv2.9:16.1_0": "Dukkhe diṭṭhe cattāri saccāni diṭṭhāni hontīti?",
  "kv2.9:16.2_0": "Āmantā.",
  "kv2.9:16.3_0": "Dukkhasaccaṁ cattāri saccānīti?",
  "kv2.9:16.4_0": "Na hevaṁ vattabbe …pe….",
  "kv2.9:17.1_0": "Rūpakkhandhe aniccato diṭṭhe pañcakkhandhā aniccato diṭṭhā hontīti?",
  "kv2.9:17.2_0": "Āmantā.",
  "kv2.9:17.3_0": "Rūpakkhandho pañcakkhandhāti?",
  "kv2.9:17.4_0": "Na hevaṁ vattabbe …pe….",
  "kv2.9:18.1_0": "Cakkhāyatane aniccato diṭṭhe dvādasāyatanāni aniccato diṭṭhāni hontīti?",
  "kv2.9:18.2_0": "Āmantā.",
  "kv2.9:18.3_0": "Cakkhāyatanaṁ dvādasāyatanānīti?",
  "kv2.9:18.4_0": "Na hevaṁ vattabbe …pe….",
  "kv2.9:19.1_0": "Cakkhudhātuyā aniccato diṭṭhāya aṭṭhārasa dhātuyo aniccato diṭṭhā hontīti?",
  "kv2.9:19.2_0": "Āmantā.",
  "kv2.9:19.3_0": "Cakkhudhātu aṭṭhārasa dhātuyoti?",
  "kv2.9:19.4_0": "Na hevaṁ vattabbe …pe….",
  "kv2.9:20.1_0": "Cakkhundriye aniccato diṭṭhe bāvīsatindriyāni aniccato diṭṭhāni hontīti?",
  "kv2.9:20.2_0": "Āmantā.",
  "kv2.9:20.3_0": "Cakkhundriyaṁ bāvīsatindriyānīti?",
  "kv2.9:20.4_0": "Na hevaṁ vattabbe …pe….",
  "kv2.9:21.1_0": "Catūhi ñāṇehi sotāpattiphalaṁ sacchikarotīti?",
  "kv2.9:21.2_0": "Āmantā.",
  "kv2.9:21.3_0": "Cattāri sotāpattiphalānīti?",
  "kv2.9:21.4_0": "Na hevaṁ vattabbe …pe…",
  "kv2.9:21.5_0": "aṭṭhahi ñāṇehi sotāpattiphalaṁ sacchikarotīti?",
  "kv2.9:21.6_0": "Āmantā.",
  "kv2.9:21.7_0": "Aṭṭha sotāpattiphalānīti?",
  "kv2.9:21.8_0": "Na hevaṁ vattabbe …pe…",
  "kv2.9:21.9_0": "dvādasahi ñāṇehi sotāpattiphalaṁ sacchikarotīti?",
  "kv2.9:21.10_0": "Āmantā.",
  "kv2.9:21.11_0": "Dvādasa sotāpattiphalānīti?",
  "kv2.9:21.12_0": "Na hevaṁ vattabbe …pe…",
  "kv2.9:21.13_0": "catucattārīsāya ñāṇehi sotāpattiphalaṁ sacchikarotīti?",
  "kv2.9:21.14_0": "Āmantā.",
  "kv2.9:21.15_0": "Catucattārīsaṁ sotāpattiphalānīti?",
  "kv2.9:21.16_0": "Na hevaṁ vattabbe …pe…",
  "kv2.9:21.17_0": "sattasattatiyā ñāṇehi sotāpattiphalaṁ sacchikarotīti?",
  "kv2.9:21.18_0": "Āmantā.",
  "kv2.9:21.19_0": "Sattasattati sotāpattiphalānīti?",
  "kv2.9:21.20_0": "Na hevaṁ vattabbe …pe….",
  "kv2.9:22.1_0": "Na vattabbaṁ—",
  "kv2.9:22.2_0": "“anupubbābhisamayo”ti?",
  "kv2.9:22.3_0": "Āmantā.",
  "kv2.9:22.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv2.9:22.5_0": "“seyyathāpi,",
  "kv2.9:22.5_1": "bhikkhave,",
  "kv2.9:22.5_2": "mahāsamuddo anupubbaninno anupubbapoṇo anupubbapabbhāro,",
  "kv2.9:22.5_3": "na āyatakeneva papāto;",
  "kv2.9:22.6_0": "evameva kho,",
  "kv2.9:22.6_1": "bhikkhave,",
  "kv2.9:22.6_2": "imasmiṁ dhammavinaye anupubbasikkhā anupubbakiriyā anupubbapaṭipadā,",
  "kv2.9:22.6_3": "na āyatakeneva aññāpaṭivedho”ti.",
  "kv2.9:22.7_0": "Attheva suttantoti?",
  "kv2.9:22.8_0": "Āmantā.",
  "kv2.9:22.9_0": "Tena hi anupubbābhisamayoti.",
  "kv2.9:23.1_0": "Na vattabbaṁ—",
  "kv2.9:23.2_0": "“anupubbābhisamayo”ti?",
  "kv2.9:23.3_0": "Āmantā.",
  "kv2.9:23.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv2.9:24.1_0": "“Anupubbena medhāvī,",
  "kv2.9:24.2_0": "thokaṁ thokaṁ khaṇe khaṇe;",
  "kv2.9:24.3_0": "Kammāro rajatasseva,",
  "kv2.9:24.4_0": "niddhame malamattano”ti.",
  "kv2.9:25.1_0": "Attheva suttantoti?",
  "kv2.9:25.2_0": "Āmantā.",
  "kv2.9:25.3_0": "Tena hi anupubbābhisamayoti.",
  "kv2.9:26.1_0": "Anupubbābhisamayoti?",
  "kv2.9:26.2_0": "Āmantā.",
  "kv2.9:26.3_0": "Nanvāyasmā gavampati thero bhikkhū etadavoca—",
  "kv2.9:26.4_0": "“sammukhā metaṁ, āvuso, bhagavato sutaṁ sammukhā paṭiggahitaṁ—",
  "kv2.9:26.5_0": "‘yo,",
  "kv2.9:26.5_1": "bhikkhave,",
  "kv2.9:26.5_2": "dukkhaṁ passati dukkhasamudayampi so passati,",
  "kv2.9:26.5_3": "dukkhanirodhampi passati,",
  "kv2.9:26.5_4": "dukkhanirodhagāminiṁ paṭipadampi passati;",
  "kv2.9:26.6_0": "yo dukkhasamudayaṁ passati dukkhampi so passati,",
  "kv2.9:26.6_1": "dukkhanirodhampi passati,",
  "kv2.9:26.6_2": "dukkhanirodhagāminiṁ paṭipadampi passati;",
  "kv2.9:26.7_0": "yo dukkhanirodhaṁ passati dukkhampi so passati,",
  "kv2.9:26.7_1": "dukkhasamudayampi passati,",
  "kv2.9:26.7_2": "dukkhanirodhagāminiṁ paṭipadampi passati;",
  "kv2.9:26.8_0": "yo dukkhanirodhagāminiṁ paṭipadaṁ passati dukkhampi so passati,",
  "kv2.9:26.8_1": "dukkhasamudayampi passati,",
  "kv2.9:26.8_2": "dukkhanirodhampi passatī’”ti.",
  "kv2.9:26.9_0": "Attheva suttantoti?",
  "kv2.9:26.10_0": "Āmantā.",
  "kv2.9:26.11_0": "Tena hi na vattabbaṁ—",
  "kv2.9:26.12_0": "“anupubbābhisamayo”ti.",
  "kv2.9:27.1_0": "Anupubbābhisamayoti?",
  "kv2.9:27.2_0": "Āmantā.",
  "kv2.9:27.3_0": "Nanu vuttaṁ bhagavatā—",
  "kv2.9:28.1_0": "“Sahāvassa dassanasampadāya,",
  "kv2.9:28.2_0": "Tayassu dhammā jahitā bhavanti;",
  "kv2.9:28.3_0": "Sakkāyadiṭṭhī vicikicchitañca,",
  "kv2.9:28.4_0": "Sīlabbataṁ vāpi yadatthi kiñci;",
  "kv2.9:28.5_0": "Catūhapāyehi ca vippamutto,",
  "kv2.9:28.6_0": "Chaccābhiṭhānāni abhabba kātun”ti.",
  "kv2.9:29.1_0": "Attheva suttantoti?",
  "kv2.9:29.2_0": "Āmantā.",
  "kv2.9:29.3_0": "Tena hi na vattabbaṁ—",
  "kv2.9:29.4_0": "“anupubbābhisamayo”ti.",
  "kv2.9:30.1_0": "Anupubbābhisamayoti?",
  "kv2.9:30.2_0": "Āmantā.",
  "kv2.9:30.3_0": "Nanu vuttaṁ bhagavatā—",
  "kv2.9:30.4_0": "“yasmiṁ, bhikkhave, samaye ariyasāvakassa virajaṁ vītamalaṁ dhammacakkhuṁ udapādi—",
  "kv2.9:30.5_0": "‘yaṁ kiñci samudayadhammaṁ sabbaṁ taṁ nirodhadhamman’ti,",
  "kv2.9:30.5_1": "saha dassanuppādā,",
  "kv2.9:30.5_2": "bhikkhave,",
  "kv2.9:30.5_3": "ariyasāvakassa tīṇi saṁyojanāni pahīyanti—",
  "kv2.9:30.6_0": "sakkāyadiṭṭhi, vicikicchā, sīlabbataparāmāso”ti.",
  "kv2.9:30.7_0": "Attheva suttantoti?",
  "kv2.9:30.8_0": "Āmantā.",
  "kv2.9:30.9_0": "Tena hi na vattabbaṁ—",
  "kv2.9:30.10_0": "“anupubbābhisamayo”ti.",
  "kv2.9:30.11_0": "Anupubbābhisamayakathā niṭṭhitā."
}