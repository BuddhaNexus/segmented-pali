{
  "kv2.9:0.1_0": "Kathāvatthu",
  "kv2.9:0.2_0": "Mahāpaṇṇāsaka",
  "kv2.9:0.3_0": "Dutiyavagga",
  "kv2.9:0.4_0": "Anupubbābhisamayakathā",
  "kv2.9:1.1_0": "Anupubbābhisamayoti?",
  "kv2.9:1.2_0": "Āmantā.",
  "kv2.9:1.3_0": "Anupubbena sotāpattimaggaṁ bhāvetīti?",
  "kv2.9:1.4_0": "Na hevaṁ vattabbe.",
  "kv2.9:1.5_0": "Anupubbena sotāpattimaggaṁ bhāvetīti?",
  "kv2.9:1.6_0": "Āmantā.",
  "kv2.9:1.7_0": "Anupubbena sotāpattiphalaṁ sacchikarotīti?",
  "kv2.9:1.8_0": "Na hevaṁ vattabbe.",
  "kv2.9:2.1_0": "Anupubbābhisamayoti?",
  "kv2.9:2.2_0": "Āmantā.",
  "kv2.9:2.3_0": "Anupubbena sakadāgāmimaggaṁ bhāvetīti?",
