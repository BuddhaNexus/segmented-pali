{
  "ja458:0.1_0": "Jātaka",
  "ja458:0.2_0": "Ekādasakanipāta",
  "ja458:0.3_0": "Mātuposakavagga",
  "ja458:0.4_0": "4. Udayajātaka",
  "ja458:1.1_0": "“Ekā nisinnā suci saññatūrū,",
  "ja458:1.2_0": "Pāsādamāruyha aninditaṅgī;",
  "ja458:1.3_0": "Yācāmi taṁ kinnaranettacakkhu,",
  "ja458:1.4_0": "Imekarattiṁ ubhayo vasema”.",
  "ja458:2.1_0": "“Okiṇṇantaraparikhaṁ,",
  "ja458:2.2_0": "daḷhamaṭṭālakoṭṭhakaṁ;",
  "ja458:2.3_0": "Rakkhitaṁ khaggahatthehi,",
  "ja458:2.4_0": "duppavesamidaṁ puraṁ.",
  "ja458:3.1_0": "Daharassa yuvino cāpi,",
  "ja458:3.2_0": "āgamo ca na vijjati;",
  "ja458:3.3_0": "Atha kena nu vaṇṇena,",
  "ja458:3.4_0": "saṅgamaṁ icchase mayā”.",
  "ja458:4.1_0": "“Yakkhohamasmi kalyāṇi,",
  "ja458:4.2_0": "Āgatosmi tavantike;",
  "ja458:4.3_0": "Tvaṁ maṁ nandaya bhaddante,",
  "ja458:4.4_0": "Puṇṇakaṁsaṁ dadāmi te”.",
  "ja458:5.1_0": "“Devaṁ va yakkhaṁ atha vā manussaṁ,",
  "ja458:5.2_0": "Na patthaye udayamaticca aññaṁ;",
  "ja458:5.3_0": "Gaccheva tvaṁ yakkha mahānubhāva,",
  "ja458:5.4_0": "Mā cassu gantvā punarāvajittha”.",
  "ja458:6.1_0": "“Yā sā rati uttamā kāmabhoginaṁ,",
  "ja458:6.2_0": "Yaṁ hetu sattā visamaṁ caranti;",
  "ja458:6.3_0": "Mā taṁ ratiṁ jīyi tuvaṁ sucimhite,",
  "ja458:6.4_0": "Dadāmi te rūpiyaṁ kaṁsapūraṁ”.",
  "ja458:7.1_0": "“Nāriṁ naro nijjhapayaṁ dhanena,",
  "ja458:7.2_0": "Ukkaṁsatī yattha karoti chandaṁ;",
  "ja458:7.3_0": "Vipaccanīko tava devadhammo,",
  "ja458:7.4_0": "Paccakkhato thokatarena esi”.",
  "ja458:8.1_0": "“Āyu ca vaṇṇo ca manussaloke,",
  "ja458:8.2_0": "Nihīyati manujānaṁ sugatte;",
  "ja458:8.3_0": "Teneva vaṇṇena dhanampi tuyhaṁ,",
  "ja458:8.4_0": "Nihīyati jiṇṇatarāsi ajja.",
  "ja458:9.1_0": "Evaṁ me pekkhamānassa,",
  "ja458:9.2_0": "rājaputti yasassini;",
  "ja458:9.3_0": "Hāyateva tava vaṇṇo,",
  "ja458:9.4_0": "ahorattānamaccaye”.",
  "ja458:10.1_0": "“Imināva tvaṁ vayasā,",
  "ja458:10.2_0": "rājaputti sumedhase;",
  "ja458:10.3_0": "Brahmacariyaṁ careyyāsi,",
  "ja458:10.4_0": "bhiyyo vaṇṇavatī siyā”.",
  "ja458:11.1_0": "“Devā na jīranti yathā manussā,",
  "ja458:11.2_0": "Gattesu tesaṁ valiyo na honti;",
  "ja458:11.3_0": "Pucchāmi taṁ yakkha mahānubhāva,",
  "ja458:11.4_0": "Kathaṁ nu devāna sarīradeho”.",
  "ja458:12.1_0": "“Devā na jīranti yathā manussā,",
  "ja458:12.2_0": "Gattesu tesaṁ valiyo na honti;",
  "ja458:12.3_0": "Suve suve bhiyyatarova tesaṁ,",
  "ja458:12.4_0": "Dibbo ca vaṇṇo vipulā ca bhogā”.",
  "ja458:13.1_0": "“Kiṁsūdha bhītā janatā anekā,",
  "ja458:13.2_0": "Maggo ca nekāyatanaṁ pavutto;",
  "ja458:13.3_0": "Pucchāmi taṁ yakkha mahānubhāva,",
  "ja458:13.4_0": "Katthaṭṭhito paralokaṁ na bhāye”.",
  "ja458:14.1_0": "“Vācaṁ manañca paṇidhāya sammā,",
  "ja458:14.2_0": "Kāyena pāpāni akubbamāno;",
  "ja458:14.3_0": "Bahunnapānaṁ gharamāvasanto,",
  "ja458:14.4_0": "Saddho mudū saṁvibhāgī vadaññū;",
  "ja458:14.5_0": "Saṅgāhako sakhilo saṇhavāco,",
  "ja458:14.6_0": "Etthaṭṭhito paralokaṁ na bhāye”.",
  "ja458:15.1_0": "“Anusāsasi maṁ yakkha,",
  "ja458:15.2_0": "yathā mātā yathā pitā;",
  "ja458:15.3_0": "Uḷāravaṇṇaṁ pucchāmi,",
  "ja458:15.4_0": "ko nu tvamasi subrahā”.",
  "ja458:16.1_0": "“Udayohamasmi kalyāṇi,",
  "ja458:16.2_0": "saṅgarattā idhāgato;",
  "ja458:16.3_0": "Āmanta kho taṁ gacchāmi,",
  "ja458:16.4_0": "muttosmi tava saṅgarā”.",
  "ja458:17.1_0": "“Sace kho tvaṁ udayosi,",
  "ja458:17.2_0": "saṅgarattā idhāgato;",
  "ja458:17.3_0": "Anusāsa maṁ rājaputta,",
  "ja458:17.4_0": "yathāssa puna saṅgamo”.",
  "ja458:18.1_0": "“Atipatati vayo khaṇo tatheva,",
  "ja458:18.2_0": "Ṭhānaṁ natthi dhuvaṁ cavanti sattā;",
  "ja458:18.3_0": "Parijiyyati addhuvaṁ sarīraṁ,",
  "ja458:18.4_0": "Udaye mā pamāda carassu dhammaṁ.",
  "ja458:19.1_0": "Kasiṇā pathavī dhanassa pūrā,",
  "ja458:19.2_0": "Ekasseva siyā anaññadheyyā;",
  "ja458:19.3_0": "Tañcāpi jahati avītarāgo,",
  "ja458:19.4_0": "Udaye mā pamāda carassu dhammaṁ.",
  "ja458:20.1_0": "Mātā ca pitā ca bhātaro ca,",
  "ja458:20.2_0": "Bhariyā yāpi dhanena hoti kītā;",
  "ja458:20.3_0": "Te cāpi jahanti aññamaññaṁ,",
  "ja458:20.4_0": "Udaye mā pamāda carassu dhammaṁ.",
  "ja458:21.1_0": "Kāyo parabhojananti ñatvā,",
  "ja458:21.2_0": "Saṁsāre sugatiñca duggatiñca;",
  "ja458:21.3_0": "Ittaravāsoti jāniyāna,",
  "ja458:21.4_0": "Udaye mā pamāda carassu dhammaṁ”.",
  "ja458:22.1_0": "“Sādhu bhāsatiyaṁ yakkho,",
  "ja458:22.2_0": "appaṁ maccāna jīvitaṁ;",
  "ja458:22.3_0": "Kasirañca parittañca,",
  "ja458:22.4_0": "tañca dukkhena saṁyutaṁ;",
  "ja458:22.5_0": "Sāhaṁ ekā pabbajissāmi,",
  "ja458:22.6_0": "hitvā kāsiṁ surundhanan”ti.",
  "ja458:23.1_0": "Udayajātakaṁ catutthaṁ."
}