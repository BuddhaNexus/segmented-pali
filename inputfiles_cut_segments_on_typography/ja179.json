{
  "ja179:0.1_0": "Jātaka",
  "ja179:0.2_0": "Dukanipāta",
  "ja179:0.3_0": "Kalyāṇavagga",
  "ja179:0.4_0": "9. Satadhammajātaka",
  "ja179:1.1_0": "“Tañca appañca ucchiṭṭhaṁ,",
  "ja179:1.2_0": "tañca kicchena no adā;",
  "ja179:1.3_0": "Sohaṁ brāhmaṇajātiko,",
  "ja179:1.4_0": "yaṁ bhuttaṁ tampi uggataṁ”.",
  "ja179:2.1_0": "“Evaṁ dhammaṁ niraṅkatvā,",
  "ja179:2.2_0": "Yo adhammena jīvati;",
  "ja179:2.3_0": "Satadhammova lābhena,",
  "ja179:2.4_0": "Laddhenapi na nandatī”ti.",
  "ja179:3.1_0": "Satadhammajātakaṁ navamaṁ."
}