{
  "mil4.5:0.1_0": "Milindapañha",
  "mil4.5:0.2_0": "Meṇḍakapañhārambhakathā",
  "mil4.5:0.3_0": "Upāsakaguṇavagga",
  "mil4.5:0.4_0": "5. Ācariyaguṇa",
  "mil4.5:1.1_0": "“Bhante nāgasena,",
  "mil4.5:1.1_1": "ayaṁ bhūmibhāgo aṭṭha mantadosavivajjito,",
  "mil4.5:1.1_2": "ahañca loke paramo mantisahāyo,",
  "mil4.5:1.1_3": "guyhamanurakkhī cāhaṁ yāvāhaṁ jīvissāmi tāva guyhamanurakkhissāmi,",
  "mil4.5:1.1_4": "aṭṭhahi ca me kāraṇehi buddhi pariṇāmaṁ gatā,",
  "mil4.5:1.1_5": "dullabho etarahi mādiso antevāsī,",
  "mil4.5:1.1_6": "sammā paṭipanne antevāsike ye ācariyānaṁ pañcavīsati ācariyaguṇā,",
  "mil4.5:1.1_7": "tehi guṇehi ācariyena sammā paṭipajjitabbaṁ.",
  "mil4.5:1.2_0": "Katame pañcavīsati guṇā?",
  "mil4.5:2.1_0": "Idha,",
  "mil4.5:2.1_1": "bhante nāgasena,",
  "mil4.5:2.1_2": "ācariyena antevāsimhi satataṁ samitaṁ ārakkhā upaṭṭhapetabbā,",
  "mil4.5:2.1_3": "asevanasevanā jānitabbā,",
  "mil4.5:2.1_4": "pamattāppamattā jānitabbā,",
  "mil4.5:2.1_5": "seyyāvakāso jānitabbo,",
  "mil4.5:2.1_6": "gelaññaṁ jānitabbaṁ,",
  "mil4.5:2.1_7": "bhojanassa laddhāladdhaṁ jānitabbaṁ,",
  "mil4.5:2.1_8": "viseso jānitabbo,",
  "mil4.5:2.1_9": "pattagataṁ saṁvibhajitabbaṁ,",
  "mil4.5:2.1_10": "assāsitabbo ‘mā bhāyi,",
  "mil4.5:2.1_11": "attho te abhikkamatī’ti,",
  "mil4.5:2.1_12": "‘iminā puggalena paṭicaratī’ti paṭicāro jānitabbo,",
  "mil4.5:2.1_13": "gāme paṭicāro jānitabbo,",
  "mil4.5:2.1_14": "vihāre paṭicāro jānitabbo,",
  "mil4.5:2.1_15": "na tena hāso davo kātabbo,",
  "mil4.5:2.1_16": "tena saha ālāpo kātabbo,",
  "mil4.5:2.1_17": "chiddaṁ disvā adhivāsetabbaṁ,",
  "mil4.5:2.1_18": "sakkaccakārinā bhavitabbaṁ,",
  "mil4.5:2.1_19": "akhaṇḍakārinā bhavitabbaṁ,",
  "mil4.5:2.1_20": "arahassakārinā bhavitabbaṁ,",
  "mil4.5:2.1_21": "niravasesakārinā bhavitabbaṁ,",
  "mil4.5:2.1_22": "‘janemimaṁ sippesū’ti janakacittaṁ upaṭṭhapetabbaṁ,",
  "mil4.5:2.1_23": "‘kathaṁ ayaṁ na parihāyeyyā’ti vaḍḍhicittaṁ upaṭṭhapetabbaṁ,",
  "mil4.5:2.1_24": "‘balavaṁ imaṁ karomi sikkhābalenā’ti cittaṁ upaṭṭhapetabbaṁ,",
  "mil4.5:2.1_25": "mettacittaṁ upaṭṭhapetabbaṁ,",
  "mil4.5:2.1_26": "āpadāsu na vijahitabbaṁ,",
  "mil4.5:2.1_27": "karaṇīye nappamajjitabbaṁ,",
  "mil4.5:2.1_28": "khalite dhammena paggahetabboti.",
  "mil4.5:2.2_0": "Ime kho, bhante,",
  "mil4.5:2.2_1": "pañcavīsati ācariyassa ācariyaguṇā,",
  "mil4.5:2.2_2": "tehi guṇehi mayi sammā paṭipajjassu,",
  "mil4.5:2.2_3": "saṁsayo me, bhante, uppanno,",
  "mil4.5:2.2_4": "atthi meṇḍakapañhā jinabhāsitā,",
  "mil4.5:2.2_5": "anāgate addhāne tattha viggaho uppajjissati,",
  "mil4.5:2.2_6": "anāgate ca addhāne dullabhā bhavissanti tumhādisā buddhimanto,",
  "mil4.5:2.2_7": "tesu me pañhesu cakkhuṁ dehi paravādānaṁ niggahāyā”ti."
}