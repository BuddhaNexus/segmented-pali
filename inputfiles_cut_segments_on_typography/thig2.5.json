{
  "thig2.5:0.1_0": "Therīgāthā 2.5",
  "thig2.5:0.2_0": "Dukanipāta",
  "thig2.5:0.3_0": "Cittātherīgāthā",
  "thig2.5:1.1_0": "“Kiñcāpi khomhi kisikā,",
  "thig2.5:1.2_0": "gilānā bāḷhadubbalā;",
  "thig2.5:1.3_0": "Daṇḍamolubbha gacchāmi,",
  "thig2.5:1.4_0": "pabbataṁ abhirūhiya.",
  "thig2.5:2.1_0": "Saṅghāṭiṁ nikkhipitvāna,",
  "thig2.5:2.2_0": "pattakañca nikujjiya;",
  "thig2.5:2.3_0": "Sele khambhesimattānaṁ,",
  "thig2.5:2.4_0": "tamokhandhaṁ padāliyā”ti.",
  "thig2.5:3.1_0": "… Cittā therī …."
}