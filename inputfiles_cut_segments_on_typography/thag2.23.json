{
  "thag2.23:0.1_0": "Theragāthā 2.23",
  "thag2.23:0.2_0": "Dukanipāta",
  "thag2.23:0.3_0": "Tatiyavagga",
  "thag2.23:0.4_0": "Sobhitattheragāthā",
  "thag2.23:1.1_0": "“Satimā paññavā bhikkhu,",
  "thag2.23:1.2_0": "āraddhabalavīriyo;",
  "thag2.23:1.3_0": "Pañca kappasatānāhaṁ,",
  "thag2.23:1.4_0": "ekarattiṁ anussariṁ.",
  "thag2.23:2.1_0": "Cattāro satipaṭṭhāne,",
  "thag2.23:2.2_0": "satta aṭṭha ca bhāvayaṁ;",
  "thag2.23:2.3_0": "Pañca kappasatānāhaṁ,",
  "thag2.23:2.4_0": "ekarattiṁ anussarin”ti.",
  "thag2.23:2.5_0": "… Sobhito thero …."
}