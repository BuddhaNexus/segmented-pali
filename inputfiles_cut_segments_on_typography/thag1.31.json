{
  "thag1.31:0.1_0": "Theragāthā 1.31",
  "thag1.31:0.2_0": "Ekakanipāta",
  "thag1.31:0.3_0": "Catutthavagga",
  "thag1.31:0.4_0": "Gahvaratīriyattheragāthā",
  "thag1.31:1.1_0": "“Phuṭṭho ḍaṁsehi makasehi,",
  "thag1.31:1.2_0": "araññasmiṁ brahāvane;",
  "thag1.31:1.3_0": "Nāgo saṅgāmasīseva,",
  "thag1.31:1.4_0": "sato tatrādhivāsaye”ti.",
  "thag1.31:1.5_0": "… Gahvaratīriyo thero …."
}