{
  "tha-ap51:0.1_0": "Therāpadāna",
  "tha-ap51:0.2_0": "Upālivagga",
  "tha-ap51:0.3_0": "9. Subhaddattheraapadāna",
  "tha-ap51:1.1_0": "“Padumuttaro lokavidū,",
  "tha-ap51:1.2_0": "āhutīnaṁ paṭiggaho;",
  "tha-ap51:1.3_0": "Janataṁ uddharitvāna,",
  "tha-ap51:1.4_0": "nibbāyati mahāyaso.",
  "tha-ap51:2.1_0": "Nibbāyante ca sambuddhe,",
  "tha-ap51:2.2_0": "dasasahassi kampatha;",
  "tha-ap51:2.3_0": "Janakāyo mahā āsi,",
  "tha-ap51:2.4_0": "devā sannipatuṁ tadā.",
  "tha-ap51:3.1_0": "Candanaṁ pūrayitvāna,",
  "tha-ap51:3.2_0": "tagarāmallikāhi ca;",
  "tha-ap51:3.3_0": "Haṭṭho haṭṭhena cittena,",
  "tha-ap51:3.4_0": "āropayiṁ naruttamaṁ.",
  "tha-ap51:4.1_0": "Mama saṅkappamaññāya,",
  "tha-ap51:4.2_0": "satthā loke anuttaro;",
  "tha-ap51:4.3_0": "Nipannakova sambuddho,",
  "tha-ap51:4.4_0": "imā gāthā abhāsatha.",
  "tha-ap51:5.1_0": "‘Yo me pacchimake kāle,",
  "tha-ap51:5.2_0": "gandhamālena chādayi;",
  "tha-ap51:5.3_0": "Tamahaṁ kittayissāmi,",
  "tha-ap51:5.4_0": "suṇātha mama bhāsato.",
  "tha-ap51:6.1_0": "Ito cuto ayaṁ poso,",
  "tha-ap51:6.2_0": "tusitakāyaṁ gamissati;",
  "tha-ap51:6.3_0": "Tattha rajjaṁ karitvāna,",
  "tha-ap51:6.4_0": "nimmānaṁ so gamissati.",
  "tha-ap51:7.1_0": "Eteneva upāyena,",
  "tha-ap51:7.2_0": "datvā mālaṁ varuttamaṁ;",
  "tha-ap51:7.3_0": "Sakakammābhiraddho so,",
  "tha-ap51:7.4_0": "sampattiṁ anubhossati.",
  "tha-ap51:8.1_0": "Punāpi tusite kāye,",
  "tha-ap51:8.2_0": "nibbattissatiyaṁ naro;",
  "tha-ap51:8.3_0": "Tamhā kāyā cavitvāna,",
  "tha-ap51:8.4_0": "manussattaṁ gamissati.",
  "tha-ap51:9.1_0": "Sakyaputto mahānāgo,",
  "tha-ap51:9.2_0": "aggo loke sadevake;",
  "tha-ap51:9.3_0": "Bodhayitvā bahū satte,",
  "tha-ap51:9.4_0": "nibbāyissati cakkhumā.",
  "tha-ap51:10.1_0": "Tadā sopagato santo,",
  "tha-ap51:10.2_0": "sukkamūlena codito;",
  "tha-ap51:10.3_0": "Upasaṅkamma sambuddhaṁ,",
  "tha-ap51:10.4_0": "pañhaṁ pucchissati tadā.",
  "tha-ap51:11.1_0": "Hāsayitvāna sambuddho,",
  "tha-ap51:11.2_0": "sabbaññū lokanāyako;",
  "tha-ap51:11.3_0": "Puññakammaṁ pariññāya,",
  "tha-ap51:11.4_0": "saccāni vivarissati.",
  "tha-ap51:12.1_0": "Āraddho ca ayaṁ pañho,",
  "tha-ap51:12.2_0": "tuṭṭho ekaggamānaso;",
  "tha-ap51:12.3_0": "Satthāraṁ abhivādetvā,",
  "tha-ap51:12.4_0": "pabbajjaṁ yācayissati.",
  "tha-ap51:13.1_0": "Pasannamānasaṁ disvā,",
  "tha-ap51:13.2_0": "sakakammena tositaṁ;",
  "tha-ap51:13.3_0": "Pabbājessati so buddho,",
  "tha-ap51:13.4_0": "aggamaggassa kovido.",
  "tha-ap51:14.1_0": "Vāyamitvānayaṁ poso,",
  "tha-ap51:14.2_0": "sammāsambuddhasāsane;",
  "tha-ap51:14.3_0": "Sabbāsave pariññāya,",
  "tha-ap51:14.4_0": "nibbāyissatināsavo’.",
  "tha-ap51:15.1_0": "Pañcamabhāṇavāraṁ.",
  "tha-ap51:16.1_0": "Pubbakammena saṁyutto,",
  "tha-ap51:16.2_0": "ekaggo susamāhito;",
  "tha-ap51:16.3_0": "Buddhassa oraso putto,",
  "tha-ap51:16.4_0": "dhammajomhi sunimmito.",
  "tha-ap51:17.1_0": "Dhammarājaṁ upagamma,",
  "tha-ap51:17.2_0": "apucchiṁ pañhamuttamaṁ;",
  "tha-ap51:17.3_0": "Kathayanto ca me pañhaṁ,",
  "tha-ap51:17.4_0": "dhammasotaṁ upānayi.",
  "tha-ap51:18.1_0": "Tassāhaṁ dhammamaññāya,",
  "tha-ap51:18.2_0": "vihāsiṁ sāsane rato;",
  "tha-ap51:18.3_0": "Sabbāsave pariññāya,",
  "tha-ap51:18.4_0": "viharāmi anāsavo.",
  "tha-ap51:19.1_0": "Satasahassito kappe,",
  "tha-ap51:19.2_0": "jalajuttamanāyako;",
  "tha-ap51:19.3_0": "Nibbāyi anupādāno,",
  "tha-ap51:19.4_0": "dīpova telasaṅkhayā.",
  "tha-ap51:20.1_0": "Sattayojanikaṁ āsi,",
  "tha-ap51:20.2_0": "thūpañca ratanāmayaṁ;",
  "tha-ap51:20.3_0": "Dhajaṁ tattha apūjesiṁ,",
  "tha-ap51:20.4_0": "sabbabhaddaṁ manoramaṁ.",
  "tha-ap51:21.1_0": "Kassapassa ca buddhassa,",
  "tha-ap51:21.2_0": "tisso nāmaggasāvako;",
  "tha-ap51:21.3_0": "Putto me oraso āsi,",
  "tha-ap51:21.4_0": "dāyādo jinasāsane.",
  "tha-ap51:22.1_0": "Tassa hīnena manasā,",
  "tha-ap51:22.2_0": "vācaṁ bhāsiṁ abhaddakaṁ;",
  "tha-ap51:22.3_0": "Tena kammavipākena,",
  "tha-ap51:22.4_0": "pacchā me āsi bhaddakaṁ.",
  "tha-ap51:23.1_0": "Upavattane sālavane,",
  "tha-ap51:23.2_0": "pacchime sayane muni;",
  "tha-ap51:23.3_0": "Pabbājesi mahāvīro,",
  "tha-ap51:23.4_0": "hito kāruṇiko jino.",
  "tha-ap51:24.1_0": "Ajjeva dāni pabbajjā,",
  "tha-ap51:24.2_0": "ajjeva upasampadā;",
  "tha-ap51:24.3_0": "Ajjeva parinibbānaṁ,",
  "tha-ap51:24.4_0": "sammukhā dvipaduttame.",
  "tha-ap51:25.1_0": "Paṭisambhidā catasso,",
  "tha-ap51:25.2_0": "…pe…",
  "tha-ap51:25.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap51:26.1_0": "Itthaṁ sudaṁ āyasmā subhaddo thero imā gāthāyo abhāsitthāti.",
  "tha-ap51:27.1_0": "Subhaddattherassāpadānaṁ navamaṁ."
}