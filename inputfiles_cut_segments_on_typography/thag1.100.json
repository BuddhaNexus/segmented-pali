{
  "thag1.100:0.1_0": "Theragāthā 1.100",
  "thag1.100:0.2_0": "Ekakanipāta",
  "thag1.100:0.3_0": "Dasamavagga",
  "thag1.100:0.4_0": "(Dutiya) Devasabhattheragāthā",
  "thag1.100:1.1_0": "“Sammappadhānasampanno,",
  "thag1.100:1.2_0": "Satipaṭṭhānagocaro;",
  "thag1.100:1.3_0": "Vimuttikusumasañchanno,",
  "thag1.100:1.4_0": "Parinibbissatyanāsavo”ti.",
  "thag1.100:1.5_0": "… Devasabho thero ….",
  "thag1.100:1.6_0": "vaggo dasamo.",
  "thag1.100:2.0_0": "Tassuddānaṁ",
  "thag1.100:2.1_0": "Paripuṇṇako ca vijayo,",
  "thag1.100:2.2_0": "erako mettajī muni;",
  "thag1.100:2.3_0": "Cakkhupālo khaṇḍasumano,",
  "thag1.100:2.4_0": "tisso ca abhayo tathā;",
  "thag1.100:2.5_0": "Uttiyo ca mahāpañño,",
  "thag1.100:2.6_0": "thero devasabhopi cāti."
}