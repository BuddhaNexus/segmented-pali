{
  "anya-e0802n5:0_0": "5. Taddhitakappa",
  "anya-e0802n5:1_0": "Aṭṭhamakaṇḍa",
  "anya-e0802n5:2_0": "344, 361.Vāṇa’pacce.",
  "anya-e0802n5:3_0": "Ṇapaccayo hoti vā “tassāpacca” miccetasmiṁ atthe.",
  "anya-e0802n5:4_0": "Vasiṭṭhassa apaccaṁ vāsiṭṭho,",
  "anya-e0802n5:4_1": "vasiṭṭhassa apaccaṁ vā,",
  "anya-e0802n5:4_2": "vasiṭṭhassa apaccaṁ vāsiṭṭhī,",
  "anya-e0802n5:4_3": "vasiṭṭhassa apaccaṁ vāsiṭṭhaṁ.",
  "anya-e0802n5:4_4": "Evaṁ bhāradvājo,",
  "anya-e0802n5:4_5": "bhāradvājī,",
  "anya-e0802n5:4_6": "bhāradvājaṁ. Gotamo, gotamī, gotamaṁ. Vāsudevo, vāsudevī,",
  "anya-e0802n5:4_7": "vāsudevaṁ. Bāladevo, bāladevī,",
  "anya-e0802n5:4_8": "bāladevaṁ.",
  "anya-e0802n5:4_9": "Vesāmitto,",
  "anya-e0802n5:4_10": "vesāmittī vesāmittaṁ.",
  "anya-e0802n5:5_0": "345, 366.Ṇāyana ṇāna vacchādito.",
  "anya-e0802n5:6_0": "Tasmā vacchādito gottagaṇato ṇāyanaṇānapaccayā honti vā “tassāpacca”miccetasmiṁ atthe.",
  "anya-e0802n5:7_0": "Vacchassa apaccaṁvacchāyano, vacchāno,",
  "anya-e0802n5:7_1": "vacchassa apaccaṁ vā,",
  "anya-e0802n5:7_2": "vacchassa apaccaṁ vacchāyanī, vacchānī,",
  "anya-e0802n5:7_3": "vacchassa apaccaṁ vacchāyanaṁ,",
  "anya-e0802n5:7_4": "vacchānaṁ.",
  "anya-e0802n5:7_5": "Sakaṭassa apaccaṁ sākaṭāyano, sākaṭāno,",
  "anya-e0802n5:7_6": "sakaṭassa apaccaṁ vā,",
  "anya-e0802n5:7_7": "sākaṭāyanī,",
  "anya-e0802n5:7_8": "sā kaṭānī,",
  "anya-e0802n5:7_9": "sākaṭāyanaṁ,",
  "anya-e0802n5:7_10": "sākaṭānaṁ.",
  "anya-e0802n5:7_11": "Evaṁ kaṇhāyano, kaṇhāno,",
  "anya-e0802n5:7_12": "kaṇhassa apaccaṁ vā.",
  "anya-e0802n5:7_13": "Kaṇhāyanī, kaṇhānī,",
  "anya-e0802n5:7_14": "kaṇhāyanaṁ, kaṇhānaṁ.",
  "anya-e0802n5:7_15": "Aggivessāyano,",
  "anya-e0802n5:7_16": "aggivessāno,",
  "anya-e0802n5:7_17": "aggivessāyanī,",
  "anya-e0802n5:7_18": "aggivessānī,",
  "anya-e0802n5:7_19": "aggivessāyanaṁ,",
  "anya-e0802n5:7_20": "aggivessānaṁ.",
  "anya-e0802n5:7_21": "Gacchāyano, gacchāno,",
  "anya-e0802n5:7_22": "gacchāyanī, gacchānī,",
  "anya-e0802n5:7_23": "gacchāyanaṁ,",
  "anya-e0802n5:7_24": "gacchānaṁ.",
  "anya-e0802n5:7_25": "Kappāyano, kappāno,",
  "anya-e0802n5:7_26": "kappāyanī, kappānī,",
  "anya-e0802n5:7_27": "kappāyanaṁ, kappānaṁ.",
  "anya-e0802n5:7_28": "Moggallāyano,",
  "anya-e0802n5:7_29": "moggallāno,",
  "anya-e0802n5:7_30": "moggallāyanī,",
  "anya-e0802n5:7_31": "moggallānī,",
  "anya-e0802n5:7_32": "moggallāyanaṁ,",
  "anya-e0802n5:7_33": "moggallānaṁ.",
  "anya-e0802n5:7_34": "Muñcāyano, muñcāno,",
  "anya-e0802n5:7_35": "muñcāyanī, muñcānī,",
  "anya-e0802n5:7_36": "muñcāyanaṁ, muñcānaṁ.",
  "anya-e0802n5:7_37": "Saṅghāyano, saṅghāno,",
  "anya-e0802n5:7_38": "saṅghāyanī, saṅghānī,",
  "anya-e0802n5:7_39": "saṅghāyanaṁ,",
  "anya-e0802n5:7_40": "saṅghānaṁ. Lomāyano, lomāno, lomāyanī, lomānī,",
  "anya-e0802n5:7_41": "lomāyanaṁ, lomānaṁ,",
  "anya-e0802n5:7_42": "sākamāyano, sākamāno,",
  "anya-e0802n5:7_43": "sākamāyanī, sākamānī,",
  "anya-e0802n5:7_44": "sākamāyanaṁ,",
  "anya-e0802n5:7_45": "sākamānaṁ. Nārāyano, nārāno, nārāyanī, nārānī,",
  "anya-e0802n5:7_46": "nārāyanaṁ, nārānaṁ.",
  "anya-e0802n5:7_47": "Corāyanocorāno, corāyanī, corānī,",
  "anya-e0802n5:7_48": "corāyanaṁ, corānaṁ,",
  "anya-e0802n5:7_49": "āvasālāyano,",
  "anya-e0802n5:7_50": "āvasālāno,",
  "anya-e0802n5:7_51": "āvasālāyanī,",
  "anya-e0802n5:7_52": "āvasālānī,",
  "anya-e0802n5:7_53": "āvasālāyanaṁ,",
  "anya-e0802n5:7_54": "āvasālānaṁ.",
  "anya-e0802n5:7_55": "Dvepāyano, dvepāno,",
  "anya-e0802n5:7_56": "dvepāyanī, dvepānī,",
  "anya-e0802n5:7_57": "dvepāyanaṁ, dvepānaṁ.",
  "anya-e0802n5:7_58": "Kuñcāyano, kuñcāno,",
  "anya-e0802n5:7_59": "kuñcāyanī, kuñcānī,",
  "anya-e0802n5:7_60": "kuñcāyanaṁ, kuñcānaṁ.",
  "anya-e0802n5:7_61": "Kaccāyano, kaccāno,",
  "anya-e0802n5:7_62": "kaccāyanī, kaccānī,",
  "anya-e0802n5:7_63": "kaccāyanaṁ, kaccānaṁ.",
  "anya-e0802n5:8_0": "346, 367.Ṇeyyokattikādīhi.",
  "anya-e0802n5:9_0": "Tehi gottagaṇehi kattikādīhi ṇeyyapaccayo hoti vā “tassāpacca”miccetasmiṁ atthe.",
  "anya-e0802n5:10_0": "Kattikāya apaccaṁ kattikeyyo,",
  "anya-e0802n5:10_1": "kattikāya apaccaṁ vā.",
  "anya-e0802n5:10_2": "Evaṁ venateyyo,",
  "anya-e0802n5:10_3": "rohiṇeyyo, gaṅgeyyo,",
  "anya-e0802n5:10_4": "kaddameyyo, nādeyyo, āleyyo, āheyo, kāmeyyo,",
  "anya-e0802n5:10_5": "suciyā apaccaṁ soceyyo, sāleyyo, bāleyyo, māleyyo, kāleyyo.",
  "anya-e0802n5:11_0": "347, 368.Ato ṇi vā.",
  "anya-e0802n5:12_0": "Tasmā akārato ṇipaccayo hoti vā “tassāpacca”miccetasmiṁ atthe.",
  "anya-e0802n5:13_0": "Dakkhassa apaccaṁ dakkhi,",
  "anya-e0802n5:13_1": "dakkhassa apaccaṁ vā.",
  "anya-e0802n5:13_2": "Duṇassa apaccaṁ doṇi,",
  "anya-e0802n5:13_3": "duṇassa apaccaṁ vā,",
  "anya-e0802n5:13_4": "evaṁ vāsavi,",
  "anya-e0802n5:13_5": "sakyaputti,",
  "anya-e0802n5:13_6": "nāṭaputti,",
  "anya-e0802n5:13_7": "dāsaputti, dāsavi, vāruṇi, gaṇḍi, bāladevi, pāvaki,",
  "anya-e0802n5:13_8": "jenadatti, buddhi, dhammi, saṅghi, kappi,",
  "anya-e0802n5:13_9": "anuruddhi.",
  "anya-e0802n5:14_0": "Vāti vikappanatthena ṇikapaccayo hoti “tassāpacca”miccetasmiṁ atthe.",
  "anya-e0802n5:14_1": "Sakyaputtassa apaccaṁ sakyaputtiko.",
  "anya-e0802n5:14_2": "Evaṁ nāṭaputtiko,",
  "anya-e0802n5:14_3": "jenadattiko.",
  "anya-e0802n5:15_0": "148, 371.Ṇavo’pakvādīhi.",
  "anya-e0802n5:16_0": "Upakuiccevamādīhi ṇavapaccayo hoti vā “tassāpacca”miccetasmiṁ atthe.",
  "anya-e0802n5:17_0": "Upakussa apaccaṁ opakavo,",
  "anya-e0802n5:17_1": "upakussa apaccaṁ vā.",
  "anya-e0802n5:17_2": "Manuno apaccaṁ mānavo,",
  "anya-e0802n5:17_3": "manuno apaccaṁ vā.",
  "anya-e0802n5:17_4": "Bhaggussa apaccaṁ bhaggavo,",
  "anya-e0802n5:17_5": "bhaggussa apaccaṁ vā,",
  "anya-e0802n5:17_6": "paṇḍussa apaccaṁ paṇḍavo,",
  "anya-e0802n5:17_7": "paṇḍussa apaccaṁ vā,",
  "anya-e0802n5:17_8": "bahussa apaccaṁ bāhavo,",
  "anya-e0802n5:17_9": "bahussa apaccaṁ vā.",
  "anya-e0802n5:18_0": "349, 372.Ṇera vidhavādito.",
  "anya-e0802n5:19_0": "Tasmā vidhavādito ṇerapaccayo hoti vā “tassāpacca”miccetasmiṁ atthe.",
  "anya-e0802n5:20_0": "Vidhavāya apaccaṁ vedhavero,",
  "anya-e0802n5:20_1": "vidhavāya apaccaṁ vā.",
  "anya-e0802n5:20_2": "Bandhukiyā apaccaṁ bandhukero,",
  "anya-e0802n5:20_3": "bandhukiyā apaccaṁ vā.",
  "anya-e0802n5:20_4": "Samaṇassa apaccaṁ sāmaṇero,",
  "anya-e0802n5:20_5": "samaṇassa apaccaṁ vā.",
  "anya-e0802n5:20_6": "Evaṁ sāmaṇerī,",
  "anya-e0802n5:20_7": "sāmaṇeraṁ, nāḷikero, nāḷikerī,",
  "anya-e0802n5:20_8": "nāḷikeraṁ.",
  "anya-e0802n5:21_0": "350, 373.Yena vā saṁsaṭṭhaṁ tarati carati vahati ṇiko.",
  "anya-e0802n5:22_0": "Yena vā saṁsaṭṭhaṁ,",
  "anya-e0802n5:22_1": "yena vā tarati,",
  "anya-e0802n5:22_2": "yena vā carati,",
  "anya-e0802n5:22_3": "yena vā vahati iccetesvatthesu ṇikapaccayo hoti vā.",
  "anya-e0802n5:23_0": "Tilena saṁsaṭṭhaṁ bhojanaṁ telikaṁ, tilena saṁsaṭṭhaṁ vā. Evaṁ goḷikaṁ, ghātikaṁ.",
  "anya-e0802n5:24_0": "Nāvāya taratīti nāviko, nāvāya tarati vā. Evaṁ oḷumpiko.",
  "anya-e0802n5:25_0": "Sakaṭena caratīti sākaṭiko, sakaṭena carati vā. Evaṁ pattiko, daṇḍiko, dhammiko, pādiko.",
  "anya-e0802n5:26_0": "Sīsena vahatīti sīsiko,",
  "anya-e0802n5:26_1": "sīsena vahati vā.",
  "anya-e0802n5:26_2": "Aṁsena vahatīti aṁsiko,",
  "anya-e0802n5:26_3": "aṁsena vahati vā.",
  "anya-e0802n5:26_4": "Evaṁ khandhiko, aṅguliko.",
  "anya-e0802n5:27_0": "Vāti vikappanatthena aññesupi ṇikapaccayo hoti.",
  "anya-e0802n5:27_1": "Rājagahe vasatīti rājagahiko,",
  "anya-e0802n5:27_2": "rājagahe vasati vā.",
  "anya-e0802n5:27_3": "Rājagahe jāto rājagahiko,",
  "anya-e0802n5:27_4": "rājagahe jāto vā.",
  "anya-e0802n5:27_5": "Evaṁ māgadhiko,",
  "anya-e0802n5:27_6": "sāvatthiko,",
  "anya-e0802n5:27_7": "kāpila vatthiko,",
  "anya-e0802n5:27_8": "pāṭaliputtiko, vesāliko.",
  "anya-e0802n5:28_0": "351, 374.Tamadhīte tenakatādi sannidhāna niyoga sippa bhaṇḍa jīvikatthesu ca.",
  "anya-e0802n5:29_0": "Tamadhīte,",
  "anya-e0802n5:29_1": "tenakatādiatthe,",
  "anya-e0802n5:29_2": "tamhi sannidhānā,",
  "anya-e0802n5:29_3": "tattha niyutto,",
  "anya-e0802n5:29_4": "tamassa sippaṁ,",
  "anya-e0802n5:29_5": "tamassa bhaṇḍaṁ,",
  "anya-e0802n5:29_6": "tamassa jīvikaṁ iccetesvatthesu ca ṇikapaccayo hoti vā.",
  "anya-e0802n5:30_0": "Vinayamadhīte venayiko, vinayamadhīte vā. Evaṁ suttantiko, ābhidhammiko, veyyākaraṇiko.",
  "anya-e0802n5:31_0": "Kāyena kataṁ kammaṁ kāyikaṁ, kāyena kataṁ kammaṁ vā. Evaṁ vācasikaṁ, mānasikaṁ.",
  "anya-e0802n5:32_0": "Sarīre sannidhānā vedanā sārīrikā, sarīre sannidhānā vā, evaṁ mānasikā.",
  "anya-e0802n5:33_0": "Dvāre niyutto dovāriko, dvāre niyutto vā. Evaṁ bhaṇḍāgāriko, nāgariko, nāvakammiko.",
  "anya-e0802n5:34_0": "Vīṇā assa sippaṁ veṇiko, vīṇā assa sippaṁ vā. Evaṁ pāṇaviko, modiṅgiko, vaṁsiko.",
  "anya-e0802n5:35_0": "Gandho assa bhaṇḍaṁ gandhiko, gandho assa bhaṇḍaṁ vā. Evaṁ teliko, goḷiko.",
  "anya-e0802n5:36_0": "Urabbhaṁ hantvā jīvatīti orabbhiko,",
  "anya-e0802n5:36_1": "urabbhaṁ hantvā jīvati vā.",
  "anya-e0802n5:36_2": "Magaṁ hantvā jīvatīti māgaviko,",
  "anya-e0802n5:36_3": "magaṁ hantvā jīvati vā.",
  "anya-e0802n5:36_4": "Evaṁ sokariko, sākuṇiko.",
  "anya-e0802n5:37_0": "Ādiggahaṇena aññatthāpi ṇika paccayo yojetabbo. Jālena hato jāliko, jālena hato vā.",
  "anya-e0802n5:38_0": "Suttena bandho suttiko, suttena bandho vā.",
  "anya-e0802n5:39_0": "Cāpo assa āvudho cāpiko, cāpo assa āvudho vā. Evaṁ tomariko, muggariko, mosaliko.",
  "anya-e0802n5:40_0": "Vāto assa ābādho vātiko, vāto assa ābādho vā. Evaṁ semhiko, pittiko.",
  "anya-e0802n5:41_0": "Buddhe pasanno buddhiko, buddhe pasanno vā. Evaṁ dhammiko, saṅghiko.",
  "anya-e0802n5:42_0": "Buddhassa santakaṁ buddhikaṁ, buddhassa santakaṁ vā. Evaṁ dhammikaṁ, saṅghikaṁ.",
  "anya-e0802n5:43_0": "Vatthena kītaṁ bhaṇḍaṁ vatthikaṁ,",
  "anya-e0802n5:43_1": "vatthena kītaṁ bhaṇḍaṁ vā.",
  "anya-e0802n5:43_2": "Evaṁ kumbhikaṁ, phālikaṁ,",
  "anya-e0802n5:43_3": "kiṁ kaṇikaṁ,",
  "anya-e0802n5:43_4": "sovaṇṇikaṁ.",
  "anya-e0802n5:44_0": "Kumbho assa parimāṇaṁ kumbhikaṁ, kumbho assa parimāṇaṁ vā.",
  "anya-e0802n5:45_0": "Kumbhassa rāsi kumbhikaṁ, kumbhassa rāsi vā.",
  "anya-e0802n5:46_0": "Kumbhaṁ arahatīti kumbhiko, kumbhaṁ arahati vā.",
  "anya-e0802n5:47_0": "Akkhena dibbatīti akkhiko,",
  "anya-e0802n5:47_1": "akkhena dibbati vā.",
  "anya-e0802n5:47_2": "Evaṁ sālākiko,",
  "anya-e0802n5:47_3": "tindukiko,",
  "anya-e0802n5:47_4": "ambaphaliko.",
  "anya-e0802n5:47_5": "Kapiṭṭhaphaliko,",
  "anya-e0802n5:47_6": "nāḷikeriko iccevamādi.",
  "anya-e0802n5:48_0": "352, 376.Ṇa rāgā tasse damaññatthesu ca.",
  "anya-e0802n5:49_0": "Ṇapaccayo hoti vā rāgamhā “tena rattaṁ” iccetasmiṁ atthe, “tassedaṁ” aññatthesu ca.",
  "anya-e0802n5:50_0": "Kasāvena rattaṁ vatthaṁ kāsāvaṁ,",
  "anya-e0802n5:50_1": "kasāvena rattaṁ vatthaṁ vā.",
  "anya-e0802n5:50_2": "Evaṁ kosumbhaṁ, hāliddaṁ, pāṭaṅgaṁ, rattaṅga,",
  "anya-e0802n5:50_3": "mañjiṭṭhaṁ, kuṅkumaṁ.",
  "anya-e0802n5:51_0": "Sūkarassa idaṁ maṁsaṁ sokaraṁ, sūkarassa idaṁ maṁsaṁ vā. Evaṁ māhisaṁ.",
  "anya-e0802n5:52_0": "Udumbarassa avidūre pavattaṁ vimānaṁ odumbaraṁ, udumbarassa avidūre pavattaṁ vimānaṁ vā.",
  "anya-e0802n5:53_0": "Vidisāya avidūre nivāso vediso, vidisāya avidūre nivāso vā.",
  "anya-e0802n5:54_0": "Mathurāya jāto māthuro, mathurāya jāto vā.",
  "anya-e0802n5:55_0": "Mathurāya āgato māthuro, mathurāya āgato vā.",
  "anya-e0802n5:56_0": "Kattikāya niyutto māso kattiko,",
  "anya-e0802n5:56_1": "kattikāya niyutto māso vā.",
  "anya-e0802n5:56_2": "Evaṁ māgasiro, phusso, māgho, phagguno, citto, vesākho, jeṭṭho, āsaḷho, sāvaṇo, bhaddo, assayujo.",
  "anya-e0802n5:57_0": "Na vuddhi nīlapītādo, paccaye saṇakārake.",
  "anya-e0802n5:58_0": "Phakāro phussasaddassa, “siro”ti sirasaṁ vade.",
  "anya-e0802n5:59_0": "Sikkhānaṁ samūho sikkho, bhikkhānaṁ samūho bhikkho. Evaṁ kāpoto, māyūro, kokilo.",
  "anya-e0802n5:60_0": "Buddhe, assa devatā buddho. Evaṁ bhaddo, māro, māhindo, vessavaṇo, yāmo, somo, nārāyaṇo.",
  "anya-e0802n5:61_0": "Saṁvaccharamadhīte saṁvaccharo.",
  "anya-e0802n5:61_1": "Evaṁ mohutto, nemitto,",
  "anya-e0802n5:61_2": "aṅgavijjo,",
  "anya-e0802n5:61_3": "veyyākaraṇo, chando, bhāsso, cando.",
  "anya-e0802n5:62_0": "Vasādānaṁ visayo deso vāsādo. Evaṁ kumbho, sākunto, ātisāro.",
  "anya-e0802n5:63_0": "Udumbarā asmiṁ padese santīti odumbaro.",
  "anya-e0802n5:63_1": "Sāgarehi nibbatto sāgaro.",
  "anya-e0802n5:63_2": "Sāgalamassa nivāso sāgalo.",
  "anya-e0802n5:63_3": "Mathurā assa nivāso māthuro.",
  "anya-e0802n5:63_4": "Mathurāya issaro māthuro.",
  "anya-e0802n5:63_5": "Iccevamādayo yojetabbā.",
  "anya-e0802n5:64_0": "353, 378.Jātādīnamimiyā ca.",
  "anya-e0802n5:65_0": "Jātaiccevamādīnamatthe imaiyapaccayā honti.",
  "anya-e0802n5:66_0": "Pacchā jāto pacchimo.",
  "anya-e0802n5:66_1": "Evaṁ antimo, majjhimo, purimo, uparimo, heṭṭhimo, gopphimo.",
  "anya-e0802n5:66_2": "Bodhisattajātiyā jāto bodhisattajātiyo,",
  "anya-e0802n5:66_3": "evaṁ assajātiyo,",
  "anya-e0802n5:66_4": "hatthijātiyo,",
  "anya-e0802n5:66_5": "manussajātiyo.",
  "anya-e0802n5:67_0": "Ādiggahaṇena niyuttatthāditopi tadassatthāditopi ima iya ika iccete paccayā honti.",
  "anya-e0802n5:67_1": "Anto niyutto antimo.",
  "anya-e0802n5:67_2": "Evaṁ antiyo, antiko.",
  "anya-e0802n5:68_0": "Putto assa atthi, tasmiṁ vā vijjatīti puttimo. Evaṁ puttiyeva, puttiko, kappimo, kappiyo, kappiko.",
  "anya-e0802n5:69_0": "Caggahaṇena kiyapaccayo hoti niyuttatthe.",
  "anya-e0802n5:69_1": "Jātiyaṁ niyutto jātikiyo,",
  "anya-e0802n5:69_2": "andhe niyutto andhakiyo,",
  "anya-e0802n5:69_3": "jātiyā andho jaccandho,",
  "anya-e0802n5:69_4": "jaccandhe niyutto jaccandhakiyo.",
  "anya-e0802n5:70_0": "354, 379.Samūhatthe kaṇa ṇā.",
  "anya-e0802n5:71_0": "Samūhatthe kaṇa ṇaiccete paccayā honti.",
  "anya-e0802n5:72_0": "Rājaputtānaṁ samūho rājaputtako.",
  "anya-e0802n5:72_1": "Evaṁ rājaputto,",
  "anya-e0802n5:72_2": "mānussako, mānusso, māyūrako, māyūro,",
  "anya-e0802n5:72_3": "māhiṁsako, māhiṁso.",
  "anya-e0802n5:73_0": "355, 380.Gāma jana bandhu sahāyādīhitā.",
  "anya-e0802n5:74_0": "Gāma jana bandhu sahāyaiccevamādīhi tāpaccayo hoti samūhatthe.",
  "anya-e0802n5:75_0": "Gāmānaṁ samūho gāmatā. Evaṁ janatā, bandhutā, sahāyatā, nagaratā.",
  "anya-e0802n5:76_0": "356, 381.Tadassa ṭhānamiyo ca.",
  "anya-e0802n5:77_0": "“Tadassa ṭhāna” miccetasmiṁ atthe iyapaccayo hoti.",
  "anya-e0802n5:78_0": "Madanassa ṭhānaṁ madaniyaṁ,",
  "anya-e0802n5:78_1": "bandhanassa ṭhānaṁ bandhaniyaṁ,",
  "anya-e0802n5:78_2": "mucchanassa ṭhānaṁ mucchaniyaṁ,",
  "anya-e0802n5:78_3": "evaṁ rajaniyaṁ,",
  "anya-e0802n5:78_4": "kamaniyaṁ,",
  "anya-e0802n5:78_5": "gamaniyaṁ,",
  "anya-e0802n5:78_6": "dussaniyaṁ,",
  "anya-e0802n5:78_7": "dassaniyaṁ.",
  "anya-e0802n5:79_0": "357, 382.Upamatthāyitattaṁ.",
  "anya-e0802n5:80_0": "Upamatthe āyitattapaccayo hoti.",
  "anya-e0802n5:81_0": "Dhūmo viya dissati aduṁ vanaṁ tadidaṁ dhūmāyitattaṁ,",
  "anya-e0802n5:81_1": "timiraṁ viya dissati aduṁ vanaṁ tadidaṁ timirāyitattaṁ.",
  "anya-e0802n5:82_0": "358, 383.Tannissitatthe lo.",
  "anya-e0802n5:83_0": "“Tannissitatthe, tadassa ṭhāna”miccetasmiṁ atthe ca lapaccayo hoti.",
  "anya-e0802n5:84_0": "Duṭṭhu nissitaṁ duṭṭhullaṁ,",
  "anya-e0802n5:84_1": "vedaṁ nissitaṁ vedallaṁ,",
  "anya-e0802n5:84_2": "duṭṭhu ṭhānaṁ duṭṭhullaṁ,",
  "anya-e0802n5:84_3": "vedassa ṭhānaṁ vedallaṁ.",
  "anya-e0802n5:85_0": "359, 384.Ālu tabbahule.",
  "anya-e0802n5:86_0": "Ālupaccayo hoti tabbahulatthe.",
  "anya-e0802n5:87_0": "Abhijjhā assa pakati abhijjhālu, abhijjhā assa bahulā vā abhijjhālu. Evaṁ sītālu, dhajālu, dayālu.",
  "anya-e0802n5:88_0": "360, 387.Ṇya tta tā bhāve tu.",
  "anya-e0802n5:89_0": "Ṇyattatāiccete paccayā honti bhāvatthe.",
  "anya-e0802n5:90_0": "Alasassa bhāvo ālasyaṁ,",
  "anya-e0802n5:90_1": "arogassa bhāvo ārogyaṁ.",
  "anya-e0802n5:90_2": "Paṁsukūlikassa bhāvo paṁsukūlikattaṁ,",
  "anya-e0802n5:90_3": "anodarikassa bhāvo anodarikattaṁ.",
  "anya-e0802n5:90_4": "Saṅgaṇikārāmassa bhāvo saṅgaṇikārāmatā,",
  "anya-e0802n5:90_5": "niddārāmassa bhāvo niddārāmatā.",
  "anya-e0802n5:91_0": "Tuggahaṇena ttanapaccayo hoti. Puthujjanattanaṁ, vedanattanaṁ.",
  "anya-e0802n5:92_0": "361, 388.Ṇavisamādīhi.",
  "anya-e0802n5:93_0": "Ṇapaccayo hoti visamādīhi “tassa bhāvo”iccetasmiṁ atthe.",
  "anya-e0802n5:94_0": "Visamassa bhāvo vesamaṁ, sucissa bhāvo socaṁ.",
  "anya-e0802n5:95_0": "362, 389.Ramaṇīyādito kaṇa.",
  "anya-e0802n5:96_0": "Ramaṇīyaiccevamādito kaṇa paccayo hoti “tassa bhāvo”iccetasmiṁ atthe.",
  "anya-e0802n5:97_0": "Ramaṇīyassa bhāvo rāmaṇīyakaṁ, manuññassa bhāvo mānuññakaṁ.",
  "anya-e0802n5:98_0": "363, 390.Visese taratamisikiyiṭṭhā.",
  "anya-e0802n5:99_0": "Visesatthe tara tama isika iya iṭṭhaiccete paccayā honti.",
  "anya-e0802n5:100_0": "Sabbe ime pāpā, ayamimesaṁ visesena pāpoti pāpataro. Evaṁ pāpatamo, pāpisiko, pāpiyo, pāpiṭṭho.",
  "anya-e0802n5:101_0": "364, 398.Tadassatthīti vī ca.",
  "anya-e0802n5:102_0": "“Tadassatthi”iccetasmiṁ atthe vīpaccayo hoti.",
  "anya-e0802n5:103_0": "Medhā yassa atthi, tasmiṁ vā vijjatītimedhāvī. Evaṁ māyāvī.",
  "anya-e0802n5:104_0": "Caggahaṇena sopaccayo hoti. Sumedhā yassa atthi, tasmiṁ vā vijjatīti sumedhaso.",
  "anya-e0802n5:105_0": "365, 399.Tapādito sī.",
  "anya-e0802n5:106_0": "Tapādito sīpaccayo hoti “tadassatthi”iccetasmiṁ atthe.",
  "anya-e0802n5:107_0": "Tapo yassa atthi tasmiṁ vā vijjatīti tapassī. Evaṁ yasassī, tejassī.",
  "anya-e0802n5:108_0": "266, 400.Daṇḍādito ikaī.",
  "anya-e0802n5:109_0": "Daṇḍādito ika īiccete paccayā honti “tadassatthi”iccetasmiṁ atthe.",
  "anya-e0802n5:110_0": "Daṇḍo yassa atthi, tasmiṁ vā vijjatīti daṇḍiko, daṇḍī. Evaṁ māliko, mālī.",
  "anya-e0802n5:111_0": "367, 401.Madhvādito ro.",
  "anya-e0802n5:112_0": "Madhuiccevamādito rapaccayo hoti “tadassatthi”iccetasmiṁ atthe.",
  "anya-e0802n5:113_0": "Madhu yassa atthi,",
  "anya-e0802n5:113_1": "tasmiṁ vā vijjatīti madhuro.",
  "anya-e0802n5:113_2": "Evaṁ kuñjaro, muggaro, mukharo, susiro, (sīsaro, sukaro, suṅkaro), subharo, suciro, ruciro.",
  "anya-e0802n5:114_0": "368, 402.Guṇādito vantu.",
  "anya-e0802n5:115_0": "Guṇaiccevamādito vantupaccayo hoti “tadassatthi”iccetasmiṁ atthe.",
  "anya-e0802n5:116_0": "Guṇo yassa atthi, tasmiṁ vā vijjatīti guṇavā. Evaṁ yasavā, dhanavā, paññavā, balavā, bhagavā.",
  "anya-e0802n5:117_0": "369, 403.Satyādīhi mantu.",
  "anya-e0802n5:118_0": "Satiiccevamādīhi mantupaccayohoti “tadassatthi” iccetasmiṁ atthe.",
  "anya-e0802n5:119_0": "Sati yassa atthi,",
  "anya-e0802n5:119_1": "tasmiṁ vā vijjatīti satimā,",
  "anya-e0802n5:119_2": "evaṁ jutimā, rucimā, thutimā, dhitimā, matimā, bhāṇumā.",
  "anya-e0802n5:120_0": "370, 405.Saddhāditoṇa.",
  "anya-e0802n5:121_0": "Saddhāiccevamādito ṇa paccayo hoti “tadassatthi”iccetasmiṁ atthe.",
  "anya-e0802n5:122_0": "Saddhā yassa atthi, tasmiṁ vā vijjatībhi saddho, evaṁ pañño, amaccharo.",
  "anya-e0802n5:123_0": "371, 404.Āyussukārāsa mantumhi.",
  "anya-e0802n5:124_0": "Āyussa anto ukāro asādeso hotimantumhi paccaye pare.",
  "anya-e0802n5:125_0": "Āyu assa atthi, tasmiṁ vā vijjatīti āyasmā.",
  "anya-e0802n5:126_0": "372, 385.Tappakativacane mayo.",
  "anya-e0802n5:127_0": "Tappakativacanatthe mayapaccayo hoti.",
  "anya-e0802n5:128_0": "Suvaṇṇena pakataṁ kammaṁ sovaṇṇamayaṁ.",
  "anya-e0802n5:128_1": "Evaṁ rūpiyamayaṁ jatumayaṁ,",
  "anya-e0802n5:128_2": "rajatamayaṁ,",
  "anya-e0802n5:128_3": "iṭṭhakamayaṁ, ayomayaṁ,",
  "anya-e0802n5:128_4": "mattikā mayaṁ,",
  "anya-e0802n5:128_5": "dārumayaṁ, gomayaṁ.",
  "anya-e0802n5:129_0": "373, 406.Saṅkhyāpūraṇe mo.",
  "anya-e0802n5:130_0": "Saṅkhyāpūraṇatthe mapaccayo hoti.",
  "anya-e0802n5:131_0": "Pañcannaṁ pūraṇo pañcamo, evaṁ sattamo, aṭṭhamo, navamo, dasamo.",
  "anya-e0802n5:132_0": "374, 408.Sachassavā.",
  "anya-e0802n5:133_0": "Chassa sakārādeso hoti vā saṅkhyāpūraṇatthe.",
  "anya-e0802n5:134_0": "Channaṁ pūraṇo saṭṭho, chaṭṭho vā.",
  "anya-e0802n5:135_0": "375, 412.Ekādito dasassī.",
  "anya-e0802n5:136_0": "Ekādito dasassa ante īpaccayo hoti vā saṅkhyāpūraṇatthe.",
  "anya-e0802n5:137_0": "Eko ca dasa ca ekādasa,",
  "anya-e0802n5:137_1": "ekādasannaṁ pūraṇī ekādasī.",
  "anya-e0802n5:137_2": "Pañcaca dasa ca pañcadasa,",
  "anya-e0802n5:137_3": "pañcadasannaṁ pūraṇī pañcadasī.",
  "anya-e0802n5:137_4": "Cattāro ca dasa ca catuddasa,",
  "anya-e0802n5:137_5": "catuddasannaṁ pūraṇī cātuddasī.",
  "anya-e0802n5:138_0": "Pūraṇeti kimatthaṁ? Ekādasa, pañcadasa.",
  "anya-e0802n5:139_0": "376, 257.Dase so niccañca.",
  "anya-e0802n5:140_0": "Dasasadde pare niccaṁ chassa so hoti.",
  "anya-e0802n5:141_0": "Soḷasa.",
  "anya-e0802n5:142_0": "377, 0.Ante niggahitañca.",
  "anya-e0802n5:143_0": "Tāsaṁ saṅkhyānaṁ ante niggahitāgamo hoti.",
  "anya-e0802n5:144_0": "Pañcadasi, cātuddasi.",
  "anya-e0802n5:145_0": "378, 414.Tica.",
  "anya-e0802n5:146_0": "Tāsaṁ saṅkhyānaṁ ante tikārāgamo hoti. Vīsati, tiṁsati.",
  "anya-e0802n5:147_0": "379, 258.La darānaṁ.",
  "anya-e0802n5:148_0": "Dakāra rakārānaṁ saṅkhyānaṁ lakārādeso hoti. Soḷasa, cattālīsaṁ.",
  "anya-e0802n5:149_0": "380, 255.Vīsati dasesu bā dvissa tu.",
  "anya-e0802n5:150_0": "Vīsati dasaiccetesu dvissa bā hoti.",
  "anya-e0802n5:151_0": "Bāvīsatindriyāni, bārasa manussā.",
  "anya-e0802n5:152_0": "Tuggahaṇena dvissa du di do ādesā ca honti. Durattaṁ, dirattaṁ, diguṇaṁ, dohaḷinī.",
  "anya-e0802n5:153_0": "381, 254.Ekādito dassa ra saṅkhyāne.",
  "anya-e0802n5:154_0": "Ekādito dasassa dakārassa rakāro hoti vā saṅkhyāne.",
  "anya-e0802n5:155_0": "Ekārasa, ekādasa, bārasa, dvādasa.",
  "anya-e0802n5:156_0": "Saṅkhyāneti kimatthaṁ? Dvādasāyatanāni.",
  "anya-e0802n5:157_0": "382, 259.Aṭṭhāditoca.",
  "anya-e0802n5:158_0": "Aṭṭhaiccevamādito ca dasasaddassa dakārassa rakārādeso hoti vā saṅkhyāne.",
  "anya-e0802n5:159_0": "Aṭṭhārasa, aṭṭhadasa.",
  "anya-e0802n5:160_0": "Aṭṭhāditoti kimatthaṁ? Pañcadasa, soḷasa.",
  "anya-e0802n5:161_0": "Saṅkhyāneti kimatthaṁ? Aṭṭhadasiko.",
  "anya-e0802n5:162_0": "383, 253.Dvekaṭṭhānamākāro vā.",
  "anya-e0802n5:163_0": "Dvi eka aṭṭhaiccetesamanto ākāro hoti vā saṅkhyāne.",
  "anya-e0802n5:164_0": "Dvādasa, ekādasa, aṭṭhārasa.",
  "anya-e0802n5:165_0": "Saṅkhyāneti kimatthaṁ? Dvidanto, ekadanto, ekacchanno, aṭṭhatthambho.",
  "anya-e0802n5:166_0": "384, 407.Catucchehī tha ṭhā.",
  "anya-e0802n5:167_0": "Catu chaiccetehi tha ṭhaiccete paccayā honti saṅkhyāpūraṇatthe.",
  "anya-e0802n5:168_0": "Catuttho, chaṭṭho.",
  "anya-e0802n5:169_0": "385, 409.Dvitīhitiyo.",
  "anya-e0802n5:170_0": "Dvi tiiccetehi tiyapaccayo hoti saṅkhyāpūraṇatthe.",
  "anya-e0802n5:171_0": "Dutiyo, tatiyo.",
  "anya-e0802n5:172_0": "386, 410.Tiye dutāpi ca.",
  "anya-e0802n5:173_0": "Dvi tiiccetesaṁ du taiccete ādesā honti tiyapaccaye pare.",
  "anya-e0802n5:174_0": "Dutiyo, tatiyo.",
  "anya-e0802n5:175_0": "Apiggahaṇena aññesupi dviiccetassa duādeso hoti. Durattaṁ.",
  "anya-e0802n5:176_0": "Caggahaṇena dviiccetassa dikāro hoti. Dirattaṁ, diguṇaṁ saṅghāṭiṁ pārupetvā.",
  "anya-e0802n5:177_0": "387, 411.Tesamaḍḍhūpapadena aḍḍhuḍḍha divaḍḍha diyaḍḍhaḍḍhatiyā.",
  "anya-e0802n5:178_0": "Tesaṁ catuttha dutiya tatiyānaṁ aḍḍhūpapadānaṁ aḍḍhaḍḍhudivaḍḍha diyaḍḍha aḍḍhatiyādesā honti,",
  "anya-e0802n5:178_1": "aḍḍhūpapadena sahanippajjante.",
  "anya-e0802n5:179_0": "Aḍḍhena catuttho aḍḍhuḍḍho,",
  "anya-e0802n5:179_1": "aḍḍhena dutiyo divaḍḍho,",
  "anya-e0802n5:179_2": "aḍḍhena dutiyo diyaḍḍho,",
  "anya-e0802n5:179_3": "aḍḍhena tatiyo aḍḍhatiyo.",
  "anya-e0802n5:180_0": "388, 68.Sarūpānamekasesvasakiṁ.",
  "anya-e0802n5:181_0": "Sarūpānaṁ padabyañjanānaṁ ekaseso hoti asakiṁ. Puriso ca puriso ca purisā.",
  "anya-e0802n5:182_0": "Sarūpānamiti kimatthaṁ? Hatthī ca asso ca ratho ca pattiko ca hatthiassarathapattikā,",
  "anya-e0802n5:183_0": "Asakinti kimatthaṁ? Puriso.",
  "anya-e0802n5:184_0": "389,",
  "anya-e0802n5:184_1": "413.Gaṇane dasassa dviticatupañcachasattaaṭṭhanavakānaṁ vī ti cattāra paññā cha sattāsanavā yosu,",
  "anya-e0802n5:184_2": "yonañcīsamāsaṁ ṭhi ri tī tuti.",
  "anya-e0802n5:185_0": "Gaṇane dasassa dvika tika catukka pañcaka chakka sattakaaṭṭhaka navakānaṁ sarūpānaṁ katekasesānaṁ yathāsaṅkhyaṁ",
  "anya-e0802n5:185_1": "vī ti cattāra paññā cha satta asa navaiccādesā honti asakiṁ yosu,",
  "anya-e0802n5:185_2": "yonañca īsaṁ āsaṁ ṭhi ri ti īti utiiccādesā honti.",
  "anya-e0802n5:185_3": "Pacchā puna nippajjante.",
  "anya-e0802n5:186_0": "Vīsaṁ, tiṁsaṁ, cattālīsaṁ, paññāsaṁ, saṭṭhi, sattari, sattati, asīti, navuti.",
  "anya-e0802n5:187_0": "Asakinti kimatthaṁ? Dasa.",
  "anya-e0802n5:188_0": "Gaṇaneti kimatthaṁ? Dasadasakā purisā.",
  "anya-e0802n5:189_0": "390, 256.Catūpapadassa lopo tuttarapadādicassa cucopi navā.",
  "anya-e0802n5:190_0": "Catūpapadassa gaṇane pariyāpannassa tukārassa lopo hoti,",
  "anya-e0802n5:190_1": "uttarapadādicakārassa cucopi ādesā honti navā.",
  "anya-e0802n5:191_0": "Cuddasa, coddasa, catuddasa.",
  "anya-e0802n5:192_0": "Apiggahaṇena anupapadassāpi padādicakārassa.",
  "anya-e0802n5:192_1": "Lopohoti navā,",
  "anya-e0802n5:192_2": "cassa cucopi honti. Tālīsaṁ,",
  "anya-e0802n5:192_3": "cattālīsaṁ,",
  "anya-e0802n5:192_4": "cuttālīsaṁ,",
  "anya-e0802n5:192_5": "cottālīsaṁ.",
  "anya-e0802n5:193_0": "391, 423.Yadanupapannā nipātanā sijjhanti.",
  "anya-e0802n5:194_0": "Ye saddā aniddiṭṭhalakkhaṇā,",
  "anya-e0802n5:194_1": "akkharapadabyañjanato,",
  "anya-e0802n5:194_2": "itthipumanapuṁsakaliṅgato,",
  "anya-e0802n5:194_3": "nāmupasagganipātato,",
  "anya-e0802n5:194_4": "abyayībhāvasamāsataddhitākhyātato,",
  "anya-e0802n5:194_5": "gaṇanasaṅkhyākālakārakappayogasaññāto,",
  "anya-e0802n5:194_6": "sandhipakativuddhilopāgamavikāraviparitato,",
  "anya-e0802n5:194_7": "vibhattivibhajanato ca,",
  "anya-e0802n5:194_8": "te nipātanā sijjhanti.",
  "anya-e0802n5:195_0": "392, 418.Dvādito ko’nekattheca.",
  "anya-e0802n5:196_0": "Dviiccevamādito kapaccayo hoti anekatthe ca, nipātanā sijjhanti.",
  "anya-e0802n5:197_0": "Satassa dvikaṁ dvisataṁ,",
  "anya-e0802n5:197_1": "satassa tikaṁ tisataṁ,",
  "anya-e0802n5:197_2": "satassa catukkaṁ catusataṁ,",
  "anya-e0802n5:197_3": "satassa pañcakaṁ pañcasataṁ,",
  "anya-e0802n5:197_4": "satassa chakkaṁ chasataṁ,",
  "anya-e0802n5:197_5": "satassa sattakaṁ sattasataṁ,",
  "anya-e0802n5:197_6": "satassa aṭṭhakaṁ aṭṭhasataṁ,",
  "anya-e0802n5:197_7": "satassa navakaṁ navasataṁ,",
  "anya-e0802n5:197_8": "satassa dasakaṁ dasasataṁ,",
  "anya-e0802n5:197_9": "sahassaṁ hoti.",
  "anya-e0802n5:198_0": "393, 415.Dasadasakaṁ sataṁ dasakānaṁ sataṁ sahassañca yomhi.",
  "anya-e0802n5:199_0": "Gaṇane pariyāpannassa dasadasakassa sataṁ hoti, satadasakassa sahassaṁ hoti yomhi pare.",
  "anya-e0802n5:200_0": "Sataṁ, sahassaṁ.",
  "anya-e0802n5:201_0": "Dvikādīnaṁ taduttarapadānañca nippajjante yathāsaṅkhyaṁ.",
  "anya-e0802n5:201_1": "Satassa dvikaṁ (tadidaṁ hoti) dvisataṁ,",
  "anya-e0802n5:201_2": "evaṁ tisataṁ,",
  "anya-e0802n5:201_3": "catusataṁ,",
  "anya-e0802n5:201_4": "pañcasataṁ, chasataṁ,",
  "anya-e0802n5:201_5": "sattasataṁ,",
  "anya-e0802n5:201_6": "aṭṭhasataṁ,",
  "anya-e0802n5:201_7": "navasataṁ,",
  "anya-e0802n5:201_8": "dasasataṁ,",
  "anya-e0802n5:201_9": "sahassaṁ hoti.",
  "anya-e0802n5:202_0": "394, 416.Yāva taduttari dasaguṇitañca.",
  "anya-e0802n5:203_0": "Yāva tāsaṁ saṅkhyānaṁ uttari dasaguṇitañca kātabbaṁ.",
  "anya-e0802n5:204_0": "Taṁ yathā?",
  "anya-e0802n5:204_1": "Dasassa gaṇanassa dasaguṇitaṁ katvā sataṁ hoti,",
  "anya-e0802n5:204_2": "satassa dasaguṇitaṁ katvā sahassaṁ hoti,",
  "anya-e0802n5:204_3": "sahassassa dasaguṇitaṁ katvā dasasahassaṁ hoti,",
  "anya-e0802n5:204_4": "dasasahassassa dasaguṇitaṁ katvāsatasahassaṁ hoti,",
  "anya-e0802n5:204_5": "satasahassassa dasaguṇitaṁ katvādasasatasahassaṁ hoti,",
  "anya-e0802n5:204_6": "dasasatasahassassadasaguṇitaṁ katvā koṭi hoti,",
  "anya-e0802n5:204_7": "koṭisatasahassassa sataguṇitaṁ katvā pakoṭi hoti.",
  "anya-e0802n5:204_8": "Evaṁ sesāpi yojetabbā.",
  "anya-e0802n5:205_0": "Caggahaṇaṁ visesanatthaṁ.",
  "anya-e0802n5:206_0": "395, 417.Sakanāmehi.",
  "anya-e0802n5:207_0": "Yāsaṁ pana saṅkhyānaṁ aniddiṭṭhanāmadheyyānaṁ sakehi sakehi nāmehi nippajjante.",
  "anya-e0802n5:208_0": "Satasahassānaṁ sataṁ koṭi,",
  "anya-e0802n5:208_1": "koṭisatasahassānaṁ sataṁ pakoṭi,",
  "anya-e0802n5:208_2": "pakoṭisatasahassānaṁ sataṁ koṭipakoṭi,",
  "anya-e0802n5:208_3": "koṭipakoṭisatasahassānaṁ sataṁ nahutaṁ,",
  "anya-e0802n5:208_4": "nahutasatasahassānaṁ sataṁ ninnahutaṁ,",
  "anya-e0802n5:208_5": "ninnahutasatasahassānaṁ sataṁ akkhobhiṇī.",
  "anya-e0802n5:208_6": "Tathā bindu, abbudaṁ,",
  "anya-e0802n5:208_7": "nirabbudaṁ, ahahaṁ, ababaṁ, aṭaṭaṁ,",
  "anya-e0802n5:208_8": "sogandhikaṁ, uppalaṁ, kumudaṁ, padumaṁ,",
  "anya-e0802n5:208_9": "puṇḍarikaṁ, kathānaṁ,",
  "anya-e0802n5:208_10": "mahākathānaṁ,",
  "anya-e0802n5:208_11": "asaṅkhyeyyaṁ.",
  "anya-e0802n5:209_0": "396, 363.Tesaṁ ṇo lopaṁ.",
  "anya-e0802n5:210_0": "Tesaṁ paccayānaṁ ṇo lopamāpajjate.",
  "anya-e0802n5:211_0": "Gotamassa apaccaṁ gotamo. Evaṁ vāsiṭṭho. Venateyyo, ālasyaṁ, ārogyaṁ.",
  "anya-e0802n5:212_0": "397, 420.Vibhāge dhā ca.",
  "anya-e0802n5:213_0": "Vibhāgatthe ca dhāpaccayo hoti.",
  "anya-e0802n5:214_0": "Ekena vibhāgena ekadhā. Evaṁ dvidhā, tidhā, catudhā, pañcadhā, chadhā.",
  "anya-e0802n5:215_0": "Ceti kimatthaṁ? Sopaccayo hoti. Suttaso, byañjanaso, padaso.",
  "anya-e0802n5:216_0": "398, 421.Sabbanāmehipakāravacane tu thā.",
  "anya-e0802n5:217_0": "Sabbanāmehi pakāravacanatthe thāpaccayo hoti.",
  "anya-e0802n5:218_0": "So pakāro tathā,",
  "anya-e0802n5:218_1": "taṁ pakāraṁ tathā,",
  "anya-e0802n5:218_2": "tena pakārena tathā,",
  "anya-e0802n5:218_3": "tassa pakārassa tathā,",
  "anya-e0802n5:218_4": "tasmā pakārā tathā,",
  "anya-e0802n5:218_5": "tassa pakārassa tathā,",
  "anya-e0802n5:218_6": "tasmiṁ pakāre tathā.",
  "anya-e0802n5:218_7": "Evaṁ yathā, sabbathā, aññathā, itarathā.",
  "anya-e0802n5:219_0": "Tuggahaṇaṁ kimatthaṁ?",
  "anya-e0802n5:219_1": "Tatthāpaccayo hoti.",
  "anya-e0802n5:219_2": "So pakāro tathatthā.",
  "anya-e0802n5:219_3": "Evaṁ yathatthā.",
  "anya-e0802n5:219_4": "Sabbathatthā,",
  "anya-e0802n5:219_5": "aññathatthā,",
  "anya-e0802n5:219_6": "itarathatthā.",
  "anya-e0802n5:220_0": "399, 422.Kimimehi thaṁ.",
  "anya-e0802n5:221_0": "Kiṁ imaiccetehi thaṁpaccayo hoti pakāravacanatthe.",
  "anya-e0802n5:222_0": "Ko pakāro kathaṁ,",
  "anya-e0802n5:222_1": "kaṁ pakāraṁ kathaṁ,",
  "anya-e0802n5:222_2": "kena pakārena kathaṁ,",
  "anya-e0802n5:222_3": "kassa pakārassa kathaṁ,",
  "anya-e0802n5:222_4": "kasmā pakārā kathaṁ.",
  "anya-e0802n5:222_5": "Kassa pakārassa kathaṁ,",
  "anya-e0802n5:222_6": "kasmiṁ pakāre kathaṁ,",
  "anya-e0802n5:222_7": "ayaṁ pakāro itthaṁ,",
  "anya-e0802n5:222_8": "imaṁ pakāraṁ itthaṁ,",
  "anya-e0802n5:222_9": "iminā pakārena itthaṁ,",
  "anya-e0802n5:222_10": "imassa pakārassa itthaṁ,",
  "anya-e0802n5:222_11": "imasmā pakārā itthaṁ,",
  "anya-e0802n5:222_12": "imassa pakārassa itthaṁ,",
  "anya-e0802n5:222_13": "imasmiṁ pakāre itthaṁ.",
  "anya-e0802n5:223_0": "400, 364.Vuddhādisarassavā’saṁyogantassa saṇe ca.",
  "anya-e0802n5:224_0": "Ādisarassa vā asaṁyogantassa ādibyañjanassa vā sarassa vuddhi hoti saṇakārake paccaye pare.",
  "anya-e0802n5:225_0": "Ābhidhammiko, venateyyo, vāsiṭṭho, ālasyaṁ, ārogyaṁ.",
  "anya-e0802n5:226_0": "Asaṁyogantasseti kimatthaṁ? Bhaggavo, manteyyo, kunteyyo.",
  "anya-e0802n5:227_0": "401, 375.Māyūnamāgamo ṭhāne.",
  "anya-e0802n5:228_0": "Iuiccetesaṁ ādibhūtānaṁ mā vuddhi hoti. Tesu ca e o vuddhāgamo hoti ṭhāne.",
  "anya-e0802n5:229_0": "Byākaraṇamadhīte veyyākaraṇiko,",
  "anya-e0802n5:229_1": "nyāyamadhīte neyyāyiko,",
  "anya-e0802n5:229_2": "byāvacchassa apaccaṁ veyyāvaccho,",
  "anya-e0802n5:229_3": "dvāre niyutto dovāriko.",
  "anya-e0802n5:230_0": "402, 377.Āttañca.",
  "anya-e0802n5:231_0": "Iuiccetesaṁ āttañca hoti, rikārāgamo ca ṭhāne.",
  "anya-e0802n5:232_0": "Isissa bhāvo ārisyaṁ,",
  "anya-e0802n5:232_1": "iṇassa bhāvo āṇyaṁ,",
  "anya-e0802n5:232_2": "usabhassa bhāvo āsabhaṁ,",
  "anya-e0802n5:232_3": "ujuno bhāvo ajjavaṁ,",
  "anya-e0802n5:232_4": "iccevamādī yojetabbā.",
  "anya-e0802n5:233_0": "Yūnamiti kimatthaṁ? Apāyesu jāto āpāyi ko.",
  "anya-e0802n5:234_0": "Ṭhāneti kimatthaṁ? Vematiko, opanayiko, opamāyiko, opāyiko.",
  "anya-e0802n5:235_0": "403, 354.Kvacādimajjhuttarānaṁ dīgharassā paccayesu ca.",
  "anya-e0802n5:236_0": "Kvaci ādimajjhauttaraiccetesaṁ dīgharassā honti paccayesu ca apaccayesu ca.",
  "anya-e0802n5:237_0": "Ādidīgho tāva – pākāro, nīvāro, pāsādo, pākaṭo, pātimokkho, pāṭikaṅkho iccevamādi.",
  "anya-e0802n5:238_0": "Majjhedīgho tāva – aṅgamāgadhiko, orabbhamāgavīko iccevamādi.",
  "anya-e0802n5:239_0": "Uttaradīgho tāva – khantī paramaṁ tapo titikkhā, añjanā giri, koṭarā vanaṁ, aṅgulī iccevamādi.",
  "anya-e0802n5:240_0": "Ādirasso tāva – pageva iccevamādi.",
  "anya-e0802n5:241_0": "Majjherasso tāva – sumedhaso suvaṇṇadharehi iccevamādi.",
  "anya-e0802n5:242_0": "Uttararasso tāva –",
  "anya-e0802n5:242_1": "bhovādi nāma so hoti,",
  "anya-e0802n5:242_2": "yathābhāvi guṇena so iccevamādi.",
  "anya-e0802n5:242_3": "Aññepi yathājinavacanānuparodhena yojetabbā.",
  "anya-e0802n5:243_0": "Caggahaṇena apaccayesu cāti atthaṁ samucceti,",
  "anya-e0802n5:244_0": "404, 370.Tesu vuddhilopāgamavikāraviparītādesā ca.",
  "anya-e0802n5:245_0": "Tesu ādimajjhuttaresu yathājinavacanānuparodhena kvaci vuddhi hoti,",
  "anya-e0802n5:245_1": "kvaci lopo hoti,",
  "anya-e0802n5:245_2": "kvaci āgamo hoti,",
  "anya-e0802n5:245_3": "kvaci vikāro hoti,",
  "anya-e0802n5:245_4": "kvaci viparīto hoti,",
  "anya-e0802n5:245_5": "kvaci ādeso hoti.",
  "anya-e0802n5:246_0": "Ādivuddhi tāva – ābhidhammiko, venateyyo iccevamādi.",
  "anya-e0802n5:247_0": "Majjhevuddhi tāva – sukhaseyyaṁ, sukhakāri dānaṁ, sukhakāri sīlaṁ iccevamādi.",
  "anya-e0802n5:248_0": "Uttaravuddhi tāva – kāliṅgo, māgadhiko, paccakkhadhammā iccevamādi.",
  "anya-e0802n5:249_0": "Ādilopo tāva – tālīsaṁ iccevamādi.",
  "anya-e0802n5:250_0": "Majjhelopo tāva – kattukāmo, kumbhakāraputto, vedallaṁ iccevamādi.",
  "anya-e0802n5:251_0": "Uttaralopo tāva – bhikkhu, bhikkhunī iccevamādi.",
  "anya-e0802n5:252_0": "Ādiāgamo tāva – vutto bhagavatā iccevamādi.",
  "anya-e0802n5:253_0": "Majjheāgamo tāva – sasīlavā, sapaññavāiccevamādi.",
  "anya-e0802n5:254_0": "Uttaraāgamo tāva – vedallaṁ iccevamādi.",
  "anya-e0802n5:255_0": "Ādivikāro tāva – ārisyaṁ, āṇyaṁ, āsabhaṁ, ajjavaṁ iccevamādi.",
  "anya-e0802n5:256_0": "Majjhevikāro tāva – varārisyaṁ, parārisyaṁ iccevamādi.",
  "anya-e0802n5:257_0": "Uttaravikāro tāva – yāni, tāni, sukhāni iccevamādi.",
  "anya-e0802n5:258_0": "Ādiviparīto tāva – uggate sūriye uggacchati iccevamādi.",
  "anya-e0802n5:259_0": "Majjheviparīto tāva – samuggacchati, samuggate sūriye iccevamādi.",
  "anya-e0802n5:260_0": "Uttaraviparīto tāva – digu, diguṇaṁ iccevamādi.",
  "anya-e0802n5:261_0": "Ādiādeso tāva – yūnaṁ iccevamādi.",
  "anya-e0802n5:262_0": "Majjheādeso tāva – nyāyogā iccevamādi.",
  "anya-e0802n5:263_0": "Uttaraādeso tāva –",
  "anya-e0802n5:263_1": "sabbaseyyo,",
  "anya-e0802n5:263_2": "sabbaseṭṭho,",
  "anya-e0802n5:263_3": "cittaṁ iccevamādi.",
  "anya-e0802n5:263_4": "Evaṁ yathājinavacanānuparodhena sabbattha yojetabbā.",
  "anya-e0802n5:264_0": "405, 365.Ayuvaṇṇānañcāyo vuddhi.",
  "anya-e0802n5:265_0": "A iti akāro,",
  "anya-e0802n5:265_1": "i īiti ivaṇṇo,",
  "anya-e0802n5:265_2": "u ūiti uvaṇṇo,",
  "anya-e0802n5:265_3": "tesaṁ akāraivaṇṇuvaṇṇānaṁ ā e ovuddhiyo honti yathāsaṅkhyaṁ,",
  "anya-e0802n5:265_4": "ā ī ūvuddhi ca.",
  "anya-e0802n5:266_0": "Ābhidhammiko, venateyyo, oḷumpiko.",
  "anya-e0802n5:267_0": "Puna vuddhiggahaṇaṁ kimatthaṁ?",
  "anya-e0802n5:267_1": "Uttarapadavuddhibhāvatthaṁ,",
  "anya-e0802n5:267_2": "aṅgamagadhehi āgatāti aṅgamāgadhikā.",
  "anya-e0802n5:267_3": "Nigamajanapadesu jātāti negamajānapadā.",
  "anya-e0802n5:267_4": "Purimajanapadesu jātāti porimajānapadā.",
  "anya-e0802n5:267_5": "Sattāhe niyuttoti sattāhikā,",
  "anya-e0802n5:267_6": "catuvijje niyuttoti cātuvijjikā.",
  "anya-e0802n5:267_7": "Iccevamādī yojetabbā.",
  "anya-e0802n5:268_0": "Vuddhiiccanena kvattho? Vuddhādisarassa vā’saṁyogantassa saṇe ca.",
  "anya-e0802n5:269_0": "Iti nāmakappe taddhitakappo aṭṭhamo kaṇḍo.",
  "anya-e0802n5:270_0": "Taddhitakappo niṭṭhito."
}