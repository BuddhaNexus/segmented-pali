{
  "mil3.4.5:0.1_0": "Milindapañha",
  "mil3.4.5:0.2_0": "Nibbānavagga",
  "mil3.4.5:0.3_0": "5. Vāyāmakaraṇapañha",
  "mil3.4.5:1.1_0": "Rājā āha—",
  "mil3.4.5:1.2_0": "“bhante nāgasena, tumhe bhaṇatha—",
  "mil3.4.5:1.3_0": "‘kinti imaṁ dukkhaṁ nirujjheyya, aññañca dukkhaṁ nuppajjeyyāti.",
  "mil3.4.5:1.4_0": "Etadatthā, mahārāja, amhākaṁ pabbajjā’ti.",
  "mil3.4.5:1.5_0": "Kiṁ paṭikacceva vāyamitena, nanu sampatte kāle vāyamitabban”ti?",
  "mil3.4.5:1.6_0": "Thero āha—",
  "mil3.4.5:1.7_0": "“sampatte kāle, mahārāja, vāyāmo akiccakaro bhavati, paṭikacceva vāyāmo kiccakaro bhavatī”ti.",
  "mil3.4.5:2.1_0": "“Opammaṁ karohī”ti.",
  "mil3.4.5:2.2_0": "“Taṁ kiṁ maññasi, mahārāja,",
  "mil3.4.5:2.2_1": "yadā tvaṁ pipāsito bhaveyyāsi,",
  "mil3.4.5:2.2_2": "tadā tvaṁ udapānaṁ khaṇāpeyyāsi,",
  "mil3.4.5:2.2_3": "taḷākaṁ khaṇāpeyyāsi ‘pānīyaṁ pivissāmī’”ti?",
  "mil3.4.5:2.3_0": "“Na hi, bhante”ti.",
  "mil3.4.5:2.4_0": "“Evameva kho, mahārāja,",
  "mil3.4.5:2.4_1": "sampatte kāle vāyāmo akiccakaro bhavati,",
  "mil3.4.5:2.4_2": "paṭikacceva vāyāmo kiccakaro bhavatī”ti.",
  "mil3.4.5:3.1_0": "“Bhiyyo opammaṁ karohī”ti.",
  "mil3.4.5:3.2_0": "“Taṁ kiṁ maññasi, mahārāja,",
  "mil3.4.5:3.2_1": "yadā tvaṁ bubhukkhito bhaveyyāsi,",
  "mil3.4.5:3.2_2": "tadā tvaṁ khettaṁ kasāpeyyāsi,",
  "mil3.4.5:3.2_3": "sāliṁ ropāpeyyāsi,",
  "mil3.4.5:3.2_4": "dhaññaṁ atiharāpeyyāsi ‘bhattaṁ bhuñjissāmī’”ti?",
  "mil3.4.5:3.3_0": "“Na hi, bhante”ti.",
  "mil3.4.5:3.4_0": "“Evameva kho, mahārāja,",
  "mil3.4.5:3.4_1": "sampatte kāle vāyāmo akiccakaro bhavati,",
  "mil3.4.5:3.4_2": "paṭikacceva vāyāmo kiccakaro bhavatī”ti.",
  "mil3.4.5:4.1_0": "“Bhiyyo opammaṁ karohī”ti.",
  "mil3.4.5:4.2_0": "“Taṁ kiṁ maññasi, mahārāja,",
  "mil3.4.5:4.2_1": "yadā te saṅgāmo paccupaṭṭhito bhaveyya,",
  "mil3.4.5:4.2_2": "tadā tvaṁ parikhaṁ khaṇāpeyyāsi,",
  "mil3.4.5:4.2_3": "pākāraṁ kārāpeyyāsi,",
  "mil3.4.5:4.2_4": "gopuraṁ kārāpeyyāsi,",
  "mil3.4.5:4.2_5": "aṭṭālakaṁ kārāpeyyāsi,",
  "mil3.4.5:4.2_6": "dhaññaṁ atiharāpeyyāsi,",
  "mil3.4.5:4.2_7": "tadā tvaṁ hatthismiṁ sikkheyyāsi,",
  "mil3.4.5:4.2_8": "assasmiṁ sikkheyyāsi,",
  "mil3.4.5:4.2_9": "rathasmiṁ sikkheyyāsi,",
  "mil3.4.5:4.2_10": "dhanusmiṁ sikkheyyāsi,",
  "mil3.4.5:4.2_11": "tharusmiṁ sikkheyyāsī”ti?",
  "mil3.4.5:4.3_0": "“Na hi, bhante”ti.",
  "mil3.4.5:4.4_0": "“Evameva kho, mahārāja,",
  "mil3.4.5:4.4_1": "sampatte kāle vāyāmo akiccakaro bhavati,",
  "mil3.4.5:4.4_2": "paṭikacceva vāyāmo kiccakaro bhavati.",
  "mil3.4.5:4.5_0": "Bhāsitampetaṁ, mahārāja bhagavatā—",
  "mil3.4.5:5.1_0": "‘Paṭikacceva taṁ kayirā,",
  "mil3.4.5:5.2_0": "yaṁ jaññā hitamattano;",
  "mil3.4.5:5.3_0": "Na sākaṭikacintāya,",
  "mil3.4.5:5.4_0": "mantā dhīro parakkame.",
  "mil3.4.5:6.1_0": "Yathā sākaṭiko maṭṭhaṁ,",
  "mil3.4.5:6.2_0": "samaṁ hitvā mahāpathaṁ;",
  "mil3.4.5:6.3_0": "Visamaṁ maggamāruyha,",
  "mil3.4.5:6.4_0": "akkhacchinnova jhāyati.",
  "mil3.4.5:7.1_0": "Evaṁ dhammā apakkamma,",
  "mil3.4.5:7.2_0": "adhammamanuvattiya;",
  "mil3.4.5:7.3_0": "Mando maccu mukhaṁ patto,",
  "mil3.4.5:7.4_0": "akkhacchinnova jhāyatī’”ti.",
  "mil3.4.5:8.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.4.5:9.1_0": "Vāyāmakaraṇapañho pañcamo."
}