{
  "thag1.101:0.1_0": "Theragāthā 1.101",
  "thag1.101:0.2_0": "Ekakanipāta",
  "thag1.101:0.3_0": "Ekādasamavagga",
  "thag1.101:0.4_0": "Belaṭṭhānikattheragāthā",
  "thag1.101:1.1_0": "“Hitvā gihittaṁ anavositatto,",
  "thag1.101:1.2_0": "Mukhanaṅgalī odariko kusīto;",
  "thag1.101:1.3_0": "Mahāvarāhova nivāpapuṭṭho,",
  "thag1.101:1.4_0": "Punappunaṁ gabbhamupeti mando”ti.",
  "thag1.101:1.5_0": "… Belaṭṭhāniko thero …."
}