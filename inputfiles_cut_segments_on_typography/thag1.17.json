{
  "thag1.17:0.1_0": "Theragāthā 1.17",
  "thag1.17:0.2_0": "Ekakanipāta",
  "thag1.17:0.3_0": "Dutiyavagga",
  "thag1.17:0.4_0": "Dāsakattheragāthā",
  "thag1.17:1.1_0": "“Middhī yadā hoti mahagghaso ca,",
  "thag1.17:1.2_0": "Niddāyitā samparivattasāyī;",
  "thag1.17:1.3_0": "Mahāvarāhova nivāpapuṭṭho,",
  "thag1.17:1.4_0": "Punappunaṁ gabbhamupeti mando”ti.",
  "thag1.17:1.5_0": "… Dāsako thero …."
}