{
  "pli-tv-pvr10:0.1_0": "Parivāra",
  "pli-tv-pvr10:0.2_0": "Gāthāsaṅgaṇika",
  "pli-tv-pvr10:0.3_0": "1. Sattanagaresupaññattasikkhāpada",
  "pli-tv-pvr10:1.1_0": "“Ekaṁsaṁ cīvaraṁ katvā,",
  "pli-tv-pvr10:1.2_0": "paggaṇhitvāna añjaliṁ;",
  "pli-tv-pvr10:1.3_0": "Āsīsamānarūpova,",
  "pli-tv-pvr10:1.4_0": "kissa tvaṁ idha māgato”.",
  "pli-tv-pvr10:2.1_0": "“Dvīsu vinayesu ye paññattā,",
  "pli-tv-pvr10:2.2_0": "Uddesaṁ āgacchanti uposathesu;",
  "pli-tv-pvr10:2.3_0": "Kati te sikkhāpadā honti,",
  "pli-tv-pvr10:2.4_0": "Katisu nagaresu paññattā”.",
  "pli-tv-pvr10:3.1_0": "“Bhaddako te ummaṅgo,",
  "pli-tv-pvr10:3.2_0": "yoniso paripucchasi;",
  "pli-tv-pvr10:3.3_0": "Taggha te ahamakkhissaṁ,",
  "pli-tv-pvr10:3.4_0": "yathāsi kusalo tathā”.",
  "pli-tv-pvr10:4.1_0": "“Dvīsu vinayesu ye paññattā,",
  "pli-tv-pvr10:4.2_0": "Uddesaṁ āgacchanti uposathesu;",
  "pli-tv-pvr10:4.3_0": "Aḍḍhuḍḍhasatāni te honti,",
  "pli-tv-pvr10:4.4_0": "Sattasu nagaresu paññattā”.",
  "pli-tv-pvr10:5.1_0": "“Katamesu sattasu nagaresu paññattā,",
  "pli-tv-pvr10:5.2_0": "Iṅgha me tvaṁ byākara naṁ;",
  "pli-tv-pvr10:5.3_0": "Taṁ vacanapathaṁ nisāmayitvā,",
  "pli-tv-pvr10:5.4_0": "Paṭipajjema hitāya no siyā”.",
  "pli-tv-pvr10:6.1_0": "“Vesāliyaṁ rājagahe,",
  "pli-tv-pvr10:6.2_0": "sāvatthiyañca āḷaviyaṁ;",
  "pli-tv-pvr10:6.3_0": "Kosambiyañca sakkesu,",
  "pli-tv-pvr10:6.4_0": "bhaggesu ceva paññattā”.",
  "pli-tv-pvr10:7.1_0": "“Kati vesāliyaṁ paññattā,",
  "pli-tv-pvr10:7.2_0": "kati rājagahe katā;",
  "pli-tv-pvr10:7.3_0": "Sāvatthiyaṁ kati honti,",
  "pli-tv-pvr10:7.4_0": "kati āḷaviyaṁ katā”.",
  "pli-tv-pvr10:8.1_0": "“Kati kosambiyaṁ paññattā,",
  "pli-tv-pvr10:8.2_0": "kati sakkesu vuccanti;",
  "pli-tv-pvr10:8.3_0": "Kati bhaggesu paññattā,",
  "pli-tv-pvr10:8.4_0": "taṁ me akkhāhi pucchito”.",
  "pli-tv-pvr10:9.1_0": "“Dasa vesāliyaṁ paññattā,",
  "pli-tv-pvr10:9.2_0": "Ekavīsa rājagahe katā;",
  "pli-tv-pvr10:9.3_0": "Chaūna tīṇisatāni,",
  "pli-tv-pvr10:9.4_0": "Sabbe sāvatthiyaṁ katā.",
  "pli-tv-pvr10:10.1_0": "Cha āḷaviyaṁ paññattā,",
  "pli-tv-pvr10:10.2_0": "aṭṭha kosambiyaṁ katā;",
  "pli-tv-pvr10:10.3_0": "Aṭṭha sakkesu vuccanti,",
  "pli-tv-pvr10:10.4_0": "tayo bhaggesu paññattā.",
  "pli-tv-pvr10:11.1_0": "Ye vesāliyaṁ paññattā,",
  "pli-tv-pvr10:11.2_0": "te suṇohi yathātathaṁ;",
  "pli-tv-pvr10:11.3_0": "Methunaviggahuttari,",
  "pli-tv-pvr10:11.4_0": "atirekañca kāḷakaṁ.",
  "pli-tv-pvr10:12.1_0": "Bhūtaṁ paramparabhattaṁ,",
  "pli-tv-pvr10:12.2_0": "dantaponena acelako;",
  "pli-tv-pvr10:12.3_0": "Bhikkhunīsu ca akkoso,",
  "pli-tv-pvr10:12.4_0": "dasete vesāliyaṁ katā.",
  "pli-tv-pvr10:13.1_0": "Ye rājagahe paññattā,",
  "pli-tv-pvr10:13.2_0": "Te suṇohi yathātathaṁ;",
  "pli-tv-pvr10:13.3_0": "Adinnādānaṁ rājagahe,",
  "pli-tv-pvr10:13.4_0": "Dve anuddhaṁsanā dvepi ca bhedā.",
  "pli-tv-pvr10:14.1_0": "Antaravāsakaṁ rūpiyaṁ suttaṁ,",
  "pli-tv-pvr10:14.2_0": "Ujjhāpanena ca pācitapiṇḍaṁ;",
  "pli-tv-pvr10:14.3_0": "Gaṇabhojanaṁ vikāle ca,",
  "pli-tv-pvr10:14.4_0": "Cārittaṁ nahānaṁ ūnavīsati.",
  "pli-tv-pvr10:15.1_0": "Cīvaraṁ datvā vosāsanti,",
  "pli-tv-pvr10:15.2_0": "Ete rājagahe katā;",
  "pli-tv-pvr10:15.3_0": "Giraggacariyā tattheva,",
  "pli-tv-pvr10:15.4_0": "Chandadānena ekavīsati.",
  "pli-tv-pvr10:16.1_0": "Ye sāvatthiyaṁ paññattā,",
  "pli-tv-pvr10:16.2_0": "Te suṇohi yathātathaṁ;",
  "pli-tv-pvr10:16.3_0": "Pārājikāni cattāri,",
  "pli-tv-pvr10:16.4_0": "Saṅghādisesā bhavanti soḷasa.",
  "pli-tv-pvr10:17.1_0": "Aniyatā ca dve honti,",
  "pli-tv-pvr10:17.2_0": "nissaggiyā catuvīsati;",
  "pli-tv-pvr10:17.3_0": "Chapaññāsasatañceva,",
  "pli-tv-pvr10:17.4_0": "khuddakāni pavuccanti.",
  "pli-tv-pvr10:18.1_0": "Dasayeva ca gārayhā,",
  "pli-tv-pvr10:18.2_0": "dvesattati ca sekhiyā;",
  "pli-tv-pvr10:18.3_0": "Chaūna tīṇisatāni,",
  "pli-tv-pvr10:18.4_0": "sabbe sāvatthiyaṁ katā.",
  "pli-tv-pvr10:19.1_0": "Ye āḷaviyaṁ paññattā,",
  "pli-tv-pvr10:19.2_0": "te suṇohi yathātathaṁ;",
  "pli-tv-pvr10:19.3_0": "Kuṭikosiyaseyyā ca,",
  "pli-tv-pvr10:19.4_0": "khaṇane gaccha devate;",
  "pli-tv-pvr10:19.5_0": "Sappāṇakañca siñcanti,",
  "pli-tv-pvr10:19.6_0": "cha ete āḷaviyaṁ katā.",
  "pli-tv-pvr10:20.1_0": "Ye kosambiyaṁ paññattā,",
  "pli-tv-pvr10:20.2_0": "te suṇohi yathātathaṁ;",
  "pli-tv-pvr10:20.3_0": "Mahāvihāro dovacassaṁ,",
  "pli-tv-pvr10:20.4_0": "aññaṁ dvāraṁ surāya ca;",
  "pli-tv-pvr10:20.5_0": "Anādariyaṁ sahadhammo,",
  "pli-tv-pvr10:20.6_0": "payopānena aṭṭhamaṁ.",
  "pli-tv-pvr10:21.1_0": "Ye sakkesu paññattā,",
  "pli-tv-pvr10:21.2_0": "te suṇohi yathātathaṁ;",
  "pli-tv-pvr10:21.3_0": "Eḷakalomāni patto ca,",
  "pli-tv-pvr10:21.4_0": "ovādo ceva bhesajjaṁ.",
  "pli-tv-pvr10:22.1_0": "Sūci āraññiko ceva,",
  "pli-tv-pvr10:22.2_0": "aṭṭhete kāpilavatthave;",
  "pli-tv-pvr10:22.3_0": "Udakasuddhiyā ovādo,",
  "pli-tv-pvr10:22.4_0": "bhikkhunīsu pavuccanti.",
  "pli-tv-pvr10:23.1_0": "Ye bhaggesu paññattā,",
  "pli-tv-pvr10:23.2_0": "te suṇohi yathātathaṁ;",
  "pli-tv-pvr10:23.3_0": "Samādahitvā visibbenti,",
  "pli-tv-pvr10:23.4_0": "sāmisena sasitthakaṁ.",
  "pli-tv-pvr10:24.1_0": "Pārājikāni cattāri,",
  "pli-tv-pvr10:24.2_0": "saṅghādisesāni bhavanti;",
  "pli-tv-pvr10:24.3_0": "Satta ca nissaggiyāni,",
  "pli-tv-pvr10:24.4_0": "aṭṭha dvattiṁsa khuddakā.",
  "pli-tv-pvr10:25.1_0": "Dve gārayhā tayo sekkhā,",
  "pli-tv-pvr10:25.2_0": "chappaññāsa sikkhāpadā;",
  "pli-tv-pvr10:25.3_0": "Chasu nagaresu paññattā,",
  "pli-tv-pvr10:25.4_0": "buddhenādiccabandhunā.",
  "pli-tv-pvr10:26.1_0": "Chaūna tīṇisatāni,",
  "pli-tv-pvr10:26.2_0": "sabbe sāvatthiyaṁ katā;",
  "pli-tv-pvr10:26.3_0": "Kāruṇikena buddhena,",
  "pli-tv-pvr10:26.4_0": "gotamena yasassinā”.",
  "pli-tv-pvr10:27.0_0": "2. Catuvipatti",
  "pli-tv-pvr10:27.1_0": "“Yaṁ taṁ pucchimha akittayi no,",
  "pli-tv-pvr10:27.2_0": "Taṁ taṁ byākataṁ anaññathā;",
  "pli-tv-pvr10:27.3_0": "Aññaṁ taṁ pucchāmi tadiṅgha brūhi,",
  "pli-tv-pvr10:27.4_0": "Garuka lahukañcāpi sāvasesaṁ;",
  "pli-tv-pvr10:27.5_0": "Anavasesaṁ duṭṭhullañca aduṭṭhullaṁ,",
  "pli-tv-pvr10:27.6_0": "Ye ca yāvatatiyakā.",
  "pli-tv-pvr10:28.1_0": "Sādhāraṇaṁ asādhāraṇaṁ,",
  "pli-tv-pvr10:28.2_0": "Vibhattiyo ca yehi samathehi sammanti;",
  "pli-tv-pvr10:28.3_0": "Sabbānipetāni viyākarohi,",
  "pli-tv-pvr10:28.4_0": "Handa vākyaṁ suṇoma te”.",
  "pli-tv-pvr10:29.1_0": "“Ekatiṁsā ye garukā,",
  "pli-tv-pvr10:29.2_0": "Aṭṭhettha anavasesā;",
  "pli-tv-pvr10:29.3_0": "Ye garukā te duṭṭhullā,",
  "pli-tv-pvr10:29.4_0": "Ye duṭṭhullā sā sīlavipatti;",
  "pli-tv-pvr10:29.5_0": "Pārājikaṁ saṅghādiseso,",
  "pli-tv-pvr10:29.6_0": "‘Sīlavipattī’ti vuccati.",
  "pli-tv-pvr10:30.1_0": "Thullaccayaṁ pācittiyā,",
  "pli-tv-pvr10:30.2_0": "Pāṭidesanīyaṁ dukkaṭaṁ;",
  "pli-tv-pvr10:30.3_0": "Dubbhāsitaṁ yo cāyaṁ,",
  "pli-tv-pvr10:30.4_0": "Akkosati hasādhippāyo;",
  "pli-tv-pvr10:30.5_0": "Ayaṁ sā ācāravipattisammatā.",
  "pli-tv-pvr10:31.1_0": "Viparītadiṭṭhiṁ gaṇhanti,",
  "pli-tv-pvr10:31.2_0": "Asaddhammehi purakkhatā;",
  "pli-tv-pvr10:31.3_0": "Abbhācikkhanti sambuddhaṁ,",
  "pli-tv-pvr10:31.4_0": "Duppaññā mohapārutā;",
  "pli-tv-pvr10:31.5_0": "Ayaṁ sā diṭṭhivipattisammatā.",
  "pli-tv-pvr10:32.1_0": "Ājīvahetu ājīvakāraṇā pāpiccho icchāpakato asantaṁ abhūtaṁ uttarimanussadhammaṁ ullapati,",
  "pli-tv-pvr10:32.1_1": "ājīvahetu ājīvakāraṇā sañcarittaṁ samāpajjati,",
  "pli-tv-pvr10:32.1_2": "ājīvahetu ājīvakāraṇā ‘yo te vihāre vasati,",
  "pli-tv-pvr10:32.1_3": "so bhikkhu arahā’ti bhaṇati,",
  "pli-tv-pvr10:32.1_4": "ājīvahetu ājīvakāraṇā bhikkhu paṇītabhojanāni attano atthāya viññāpetvā bhuñjati,",
  "pli-tv-pvr10:32.1_5": "ājīvahetu ājīvakāraṇā bhikkhunī paṇītabhojanāni attano atthāya viññāpetvā bhuñjati,",
  "pli-tv-pvr10:32.1_6": "ājīvahetu ājīvakāraṇā sūpaṁ vā odanaṁ vā agilāno attano atthāya viññāpetvā bhuñjati.",
  "pli-tv-pvr10:32.2_0": "Ayaṁ sā ājīvavipatti sammatā.",
  "pli-tv-pvr10:33.1_0": "Ekādasa yāvatatiyakā,",
  "pli-tv-pvr10:33.2_0": "Te suṇohi yathātathaṁ;",
  "pli-tv-pvr10:33.3_0": "Ukkhittānuvattikā,",
  "pli-tv-pvr10:33.4_0": "Aṭṭha yāvatatiyakā;",
  "pli-tv-pvr10:33.5_0": "Ariṭṭho caṇḍakāḷī ca,",
  "pli-tv-pvr10:33.6_0": "Ime te yāvatatiyakā”.",
  "pli-tv-pvr10:34.0_0": "3. Chedanakādi",
  "pli-tv-pvr10:34.1_0": "“Kati chedanakāni?",
  "pli-tv-pvr10:34.2_0": "Kati bhedanakāni?",
  "pli-tv-pvr10:34.3_0": "Kati uddālanakāni?",
  "pli-tv-pvr10:34.4_0": "Kati anaññapācittiyāni?",
  "pli-tv-pvr10:34.5_0": "Kati bhikkhusammutiyo?",
  "pli-tv-pvr10:34.6_0": "Kati sāmīciyo?",
  "pli-tv-pvr10:34.7_0": "Kati paramāni?",
  "pli-tv-pvr10:35.1_0": "Kati jānanti paññattā,",
  "pli-tv-pvr10:35.2_0": "buddhenādiccabandhunā”.",
  "pli-tv-pvr10:36.1_0": "“Cha chedanakāni.",
  "pli-tv-pvr10:36.2_0": "Ekaṁ bhedanakaṁ.",
  "pli-tv-pvr10:36.3_0": "Ekaṁ uddālanakaṁ.",
  "pli-tv-pvr10:36.4_0": "Cattāri anaññapācittiyāni.",
  "pli-tv-pvr10:36.5_0": "Catasso bhikkhusammutiyo.",
  "pli-tv-pvr10:36.6_0": "Satta sāmīciyo.",
  "pli-tv-pvr10:36.7_0": "Cuddasa paramāni.",
  "pli-tv-pvr10:37.1_0": "Sodasa jānanti paññattā,",
  "pli-tv-pvr10:37.2_0": "buddhenādiccabandhunā”.",
  "pli-tv-pvr10:38.0_0": "4. Asādhāraṇādi",
  "pli-tv-pvr10:38.1_0": "“Vīsaṁ dve satāni,",
  "pli-tv-pvr10:38.2_0": "Bhikkhūnaṁ sikkhāpadāni;",
  "pli-tv-pvr10:38.3_0": "Uddesaṁ āgacchanti uposathesu,",
  "pli-tv-pvr10:38.4_0": "Tīṇi satāni cattāri;",
  "pli-tv-pvr10:38.5_0": "Bhikkhunīnaṁ sikkhāpadāni,",
  "pli-tv-pvr10:38.6_0": "Uddesaṁ āgacchanti uposathesu.",
  "pli-tv-pvr10:39.1_0": "Chacattārīsā bhikkhūnaṁ,",
  "pli-tv-pvr10:39.2_0": "bhikkhunīhi asādhāraṇā;",
  "pli-tv-pvr10:39.3_0": "Sataṁ tiṁsā ca bhikkhunīnaṁ,",
  "pli-tv-pvr10:39.4_0": "bhikkhūhi asādhāraṇā.",
  "pli-tv-pvr10:40.1_0": "Sataṁ sattati chacceva,",
  "pli-tv-pvr10:40.2_0": "ubhinnaṁ asādhāraṇā;",
  "pli-tv-pvr10:40.3_0": "Sataṁ sattati cattāri,",
  "pli-tv-pvr10:40.4_0": "ubhinnaṁ samasikkhatā.",
  "pli-tv-pvr10:41.1_0": "Vīsaṁ dve satāni,",
  "pli-tv-pvr10:41.2_0": "Bhikkhūnaṁ sikkhāpadāni;",
  "pli-tv-pvr10:41.3_0": "Uddesaṁ āgacchanti uposathesu,",
  "pli-tv-pvr10:41.4_0": "Te suṇohi yathātathaṁ.",
  "pli-tv-pvr10:42.1_0": "Pārājikāni cattāri,",
  "pli-tv-pvr10:42.2_0": "Saṅghādisesāni bhavanti terasa;",
  "pli-tv-pvr10:42.3_0": "Aniyatā dve honti.",
  "pli-tv-pvr10:43.1_0": "Nissaggiyāni tiṁseva,",
  "pli-tv-pvr10:43.2_0": "dvenavuti ca khuddakā;",
  "pli-tv-pvr10:43.3_0": "Cattāro pāṭidesanīyā,",
  "pli-tv-pvr10:43.4_0": "pañcasattati sekhiyā.",
  "pli-tv-pvr10:44.1_0": "Vīsaṁ dve satāni cime honti,",
  "pli-tv-pvr10:44.2_0": "Bhikkhūnaṁ sikkhāpadāni;",
  "pli-tv-pvr10:44.3_0": "Uddesaṁ āgacchanti uposathesu.",
  "pli-tv-pvr10:45.1_0": "Tīṇi satāni cattāri,",
  "pli-tv-pvr10:45.2_0": "Bhikkhunīnaṁ sikkhāpadāni;",
  "pli-tv-pvr10:45.3_0": "Uddesaṁ āgacchanti uposathesu,",
  "pli-tv-pvr10:45.4_0": "Te suṇohi yathātathaṁ.",
  "pli-tv-pvr10:46.1_0": "Pārājikāni aṭṭha,",
  "pli-tv-pvr10:46.2_0": "Saṅghādisesāni bhavanti sattarasa;",
  "pli-tv-pvr10:46.3_0": "Nissaggiyāni tiṁseva,",
  "pli-tv-pvr10:46.4_0": "Sataṁ saṭṭhi cha ceva;",
  "pli-tv-pvr10:46.5_0": "Khuddakāni pavuccanti.",
  "pli-tv-pvr10:47.1_0": "Aṭṭha pāṭidesanīyā,",
  "pli-tv-pvr10:47.2_0": "Pañcasattati sekhiyā;",
  "pli-tv-pvr10:47.3_0": "Tīṇi satāni cattāri cime honti,",
  "pli-tv-pvr10:47.4_0": "Bhikkhunīnaṁ sikkhāpadāni;",
  "pli-tv-pvr10:47.5_0": "Uddesaṁ āgacchanti uposathesu.",
  "pli-tv-pvr10:48.1_0": "Chacattārīsā bhikkhūnaṁ,",
  "pli-tv-pvr10:48.2_0": "bhikkhunīhi asādhāraṇā;",
  "pli-tv-pvr10:48.3_0": "Te suṇohi yathātathaṁ.",
  "pli-tv-pvr10:49.1_0": "Cha saṅghādisesā,",
  "pli-tv-pvr10:49.2_0": "dve aniyatehi aṭṭha;",
  "pli-tv-pvr10:49.3_0": "Nissaggiyāni dvādasa,",
  "pli-tv-pvr10:49.4_0": "tehi te honti vīsati.",
  "pli-tv-pvr10:50.1_0": "Dvevīsati khuddakā,",
  "pli-tv-pvr10:50.2_0": "Caturo pāṭidesanīyā;",
  "pli-tv-pvr10:50.3_0": "Chacattārīsā cime honti,",
  "pli-tv-pvr10:50.4_0": "Bhikkhūnaṁ bhikkhunīhi asādhāraṇā.",
  "pli-tv-pvr10:51.1_0": "Sataṁ tiṁsā ca bhikkhunīnaṁ,",
  "pli-tv-pvr10:51.2_0": "bhikkhūhi asādhāraṇā;",
  "pli-tv-pvr10:51.3_0": "Te suṇohi yathātathaṁ.",
  "pli-tv-pvr10:52.1_0": "Pārājikāni cattāri,",
  "pli-tv-pvr10:52.2_0": "saṅghamhā dasa nissare;",
  "pli-tv-pvr10:52.3_0": "Nissaggiyāni dvādasa,",
  "pli-tv-pvr10:52.4_0": "channavuti ca khuddakā;",
  "pli-tv-pvr10:52.5_0": "Aṭṭha pāṭidesanīyā.",
  "pli-tv-pvr10:53.1_0": "Sataṁ tiṁsā cime honti,",
  "pli-tv-pvr10:53.2_0": "Bhikkhunīnaṁ bhikkhūhi asādhāraṇā;",
  "pli-tv-pvr10:53.3_0": "Sataṁ sattati chacceva,",
  "pli-tv-pvr10:53.4_0": "Ubhinnaṁ asādhāraṇā;",
  "pli-tv-pvr10:53.5_0": "Te suṇohi yathātathaṁ.",
  "pli-tv-pvr10:54.1_0": "Pārājikāni cattāri,",
  "pli-tv-pvr10:54.2_0": "Saṅghādisesāni bhavanti soḷasa;",
  "pli-tv-pvr10:54.3_0": "Aniyatā dve honti,",
  "pli-tv-pvr10:54.4_0": "Nissaggiyāni catuvīsati;",
  "pli-tv-pvr10:54.5_0": "Sataṁ aṭṭhārasā ceva,",
  "pli-tv-pvr10:54.6_0": "Khuddakāni pavuccanti;",
  "pli-tv-pvr10:54.7_0": "Dvādasa pāṭidesanīyā.",
  "pli-tv-pvr10:55.1_0": "Sataṁ sattati chaccevime honti,",
  "pli-tv-pvr10:55.2_0": "Ubhinnaṁ asādhāraṇā;",
  "pli-tv-pvr10:55.3_0": "Sataṁ sattati cattāri,",
  "pli-tv-pvr10:55.4_0": "Ubhinnaṁ samasikkhatā;",
  "pli-tv-pvr10:55.5_0": "Te suṇohi yathātathaṁ.",
  "pli-tv-pvr10:56.1_0": "Pārājikāni cattāri,",
  "pli-tv-pvr10:56.2_0": "Saṅghādisesāni bhavanti satta;",
  "pli-tv-pvr10:56.3_0": "Nissaggiyāni aṭṭhārasa,",
  "pli-tv-pvr10:56.4_0": "Samasattati khuddakā;",
  "pli-tv-pvr10:56.5_0": "Pañcasattati sekhiyāni.",
  "pli-tv-pvr10:57.1_0": "Sataṁ sattati cattāri cime honti,",
  "pli-tv-pvr10:57.2_0": "Ubhinnaṁ samasikkhatā;",
  "pli-tv-pvr10:57.3_0": "Aṭṭheva pārājikā ye durāsadā,",
  "pli-tv-pvr10:57.4_0": "Tālavatthusamūpamā.",
  "pli-tv-pvr10:58.1_0": "Paṇḍupalāso puthusilā,",
  "pli-tv-pvr10:58.2_0": "sīsacchinnova so naro;",
  "pli-tv-pvr10:58.3_0": "Tālova matthakacchinno,",
  "pli-tv-pvr10:58.4_0": "aviruḷhī bhavanti te.",
  "pli-tv-pvr10:59.1_0": "Tevīsati saṅghādisesā,",
  "pli-tv-pvr10:59.2_0": "Dve aniyatā;",
  "pli-tv-pvr10:59.3_0": "Dve cattārīsa nissaggiyā,",
  "pli-tv-pvr10:59.4_0": "Aṭṭhāsītisataṁ pācittiyā;",
  "pli-tv-pvr10:59.5_0": "Dvādasa pāṭidesanīyā.",
  "pli-tv-pvr10:60.1_0": "Pañcasattati sekhiyā,",
  "pli-tv-pvr10:60.2_0": "tīhi samathehi sammanti;",
  "pli-tv-pvr10:60.3_0": "Sammukhā ca paṭiññāya,",
  "pli-tv-pvr10:60.4_0": "tiṇavatthārakena ca.",
  "pli-tv-pvr10:61.1_0": "Dve uposathā dve pavāraṇā,",
  "pli-tv-pvr10:61.2_0": "Cattāri kammāni jinena desitā;",
  "pli-tv-pvr10:61.3_0": "Pañceva uddesā caturo bhavanti,",
  "pli-tv-pvr10:61.4_0": "Anaññathā āpattikkhandhā ca bhavanti satta.",
  "pli-tv-pvr10:62.1_0": "Adhikaraṇāni cattāri,",
  "pli-tv-pvr10:62.2_0": "Sattahi samathehi sammanti;",
  "pli-tv-pvr10:62.3_0": "Dvīhi catūhi tīhi,",
  "pli-tv-pvr10:62.4_0": "Kiccaṁ ekena sammati”.",
  "pli-tv-pvr10:63.0_0": "5. Pārājikādiāpatti",
  "pli-tv-pvr10:63.1_0": "“Pārājikan”ti yaṁ vuttaṁ,",
  "pli-tv-pvr10:63.2_0": "taṁ suṇohi yathātathaṁ;",
  "pli-tv-pvr10:63.3_0": "Cuto paraddho bhaṭṭho ca,",
  "pli-tv-pvr10:63.4_0": "saddhammā hi niraṅkato;",
  "pli-tv-pvr10:63.5_0": "Saṁvāsopi tahiṁ natthi,",
  "pli-tv-pvr10:63.6_0": "tenetaṁ iti vuccati.",
  "pli-tv-pvr10:64.1_0": "“Saṅghādiseso”ti yaṁ vuttaṁ,",
  "pli-tv-pvr10:64.2_0": "taṁ suṇohi yathātathaṁ;",
  "pli-tv-pvr10:64.3_0": "Saṅghova deti parivāsaṁ,",
  "pli-tv-pvr10:64.4_0": "mūlāya paṭikassati;",
  "pli-tv-pvr10:64.5_0": "Mānattaṁ deti abbheti,",
  "pli-tv-pvr10:64.6_0": "tenetaṁ iti vuccati.",
  "pli-tv-pvr10:65.1_0": "“Aniyato”ti yaṁ vuttaṁ,",
  "pli-tv-pvr10:65.2_0": "taṁ suṇohi yathātathaṁ;",
  "pli-tv-pvr10:65.3_0": "Aniyato na niyato,",
  "pli-tv-pvr10:65.4_0": "anekaṁsikataṁ padaṁ;",
  "pli-tv-pvr10:65.5_0": "Tiṇṇamaññataraṁ ṭhānaṁ,",
  "pli-tv-pvr10:65.6_0": "“aniyato”ti pavuccati.",
  "pli-tv-pvr10:66.1_0": "“Thullaccayan”ti yaṁ vuttaṁ,",
  "pli-tv-pvr10:66.2_0": "taṁ suṇohi yathātathaṁ;",
  "pli-tv-pvr10:66.3_0": "Ekassa mūle yo deseti,",
  "pli-tv-pvr10:66.4_0": "yo ca taṁ paṭigaṇhati;",
  "pli-tv-pvr10:66.5_0": "Accayo tena samo natthi,",
  "pli-tv-pvr10:66.6_0": "tenetaṁ iti vuccati.",
  "pli-tv-pvr10:67.1_0": "“Nissaggiyan”ti yaṁ vuttaṁ,",
  "pli-tv-pvr10:67.2_0": "taṁ suṇohi yathātathaṁ;",
  "pli-tv-pvr10:67.3_0": "Saṅghamajjhe gaṇamajjhe,",
  "pli-tv-pvr10:67.4_0": "ekasseva ca ekato;",
  "pli-tv-pvr10:67.5_0": "Nissajjitvāna deseti,",
  "pli-tv-pvr10:67.6_0": "tenetaṁ iti vuccati.",
  "pli-tv-pvr10:68.1_0": "“Pācittiyan”ti yaṁ vuttaṁ,",
  "pli-tv-pvr10:68.2_0": "Taṁ suṇohi yathātathaṁ;",
  "pli-tv-pvr10:68.3_0": "Pāteti kusalaṁ dhammaṁ,",
  "pli-tv-pvr10:68.4_0": "Ariyamaggaṁ aparajjhati;",
  "pli-tv-pvr10:68.5_0": "Cittasammohanaṭṭhānaṁ,",
  "pli-tv-pvr10:68.6_0": "Tenetaṁ iti vuccati.",
  "pli-tv-pvr10:69.1_0": "“Pāṭidesanīyan”ti yaṁ vuttaṁ,",
  "pli-tv-pvr10:69.2_0": "Taṁ suṇohi yathātathaṁ;",
  "pli-tv-pvr10:69.3_0": "Bhikkhu aññātako santo,",
  "pli-tv-pvr10:69.4_0": "Kicchā laddhāya bhojanaṁ;",
  "pli-tv-pvr10:69.5_0": "Sāmaṁ gahetvā bhuñjeyya,",
  "pli-tv-pvr10:69.6_0": "“Gārayhan”ti pavuccati.",
  "pli-tv-pvr10:70.1_0": "Nimantanāsu bhuñjantā chandāya,",
  "pli-tv-pvr10:70.2_0": "Vosāsati tattha bhikkhuniṁ;",
  "pli-tv-pvr10:70.3_0": "Anivāretvā tahiṁ bhuñje,",
  "pli-tv-pvr10:70.4_0": "“Gārayhan”ti pavuccati.",
  "pli-tv-pvr10:71.1_0": "Saddhācittaṁ kulaṁ gantvā,",
  "pli-tv-pvr10:71.2_0": "appabhogaṁ anāḷiyaṁ;",
  "pli-tv-pvr10:71.3_0": "Agilāno tahiṁ bhuñje,",
  "pli-tv-pvr10:71.4_0": "“gārayhan”ti pavuccati.",
  "pli-tv-pvr10:72.1_0": "Yo ce araññe viharanto,",
  "pli-tv-pvr10:72.2_0": "sāsaṅke sabhayānake;",
  "pli-tv-pvr10:72.3_0": "Aviditaṁ tahiṁ bhuñje,",
  "pli-tv-pvr10:72.4_0": "“gārayhan”ti pavuccati.",
  "pli-tv-pvr10:73.1_0": "Bhikkhunī aññātikā santā,",
  "pli-tv-pvr10:73.2_0": "Yaṁ paresaṁ mamāyitaṁ;",
  "pli-tv-pvr10:73.3_0": "Sappi telaṁ madhuṁ phāṇitaṁ,",
  "pli-tv-pvr10:73.4_0": "Macchamaṁsaṁ atho khīraṁ;",
  "pli-tv-pvr10:73.5_0": "Dadhiṁ sayaṁ viññāpeyya bhikkhunī,",
  "pli-tv-pvr10:73.6_0": "Gārayhapattā sugatassa sāsane.",
  "pli-tv-pvr10:74.1_0": "“Dukkaṭan”ti yaṁ vuttaṁ,",
  "pli-tv-pvr10:74.2_0": "taṁ suṇohi yathātathaṁ;",
  "pli-tv-pvr10:74.3_0": "Aparaddhaṁ viraddhañca,",
  "pli-tv-pvr10:74.4_0": "khalitaṁ yañca dukkaṭaṁ.",
  "pli-tv-pvr10:75.1_0": "Yaṁ manusso kare pāpaṁ,",
  "pli-tv-pvr10:75.2_0": "āvi vā yadi vā raho;",
  "pli-tv-pvr10:75.3_0": "“Dukkaṭan”ti pavedenti,",
  "pli-tv-pvr10:75.4_0": "tenetaṁ iti vuccati.",
  "pli-tv-pvr10:76.1_0": "“Dubbhāsitan”ti yaṁ vuttaṁ,",
  "pli-tv-pvr10:76.2_0": "taṁ suṇohi yathātathaṁ;",
  "pli-tv-pvr10:76.3_0": "Dubbhāsitaṁ durābhaṭṭhaṁ,",
  "pli-tv-pvr10:76.4_0": "saṅkiliṭṭhañca yaṁ padaṁ;",
  "pli-tv-pvr10:76.5_0": "Yañca viññū garahanti,",
  "pli-tv-pvr10:76.6_0": "tenetaṁ iti vuccati.",
  "pli-tv-pvr10:77.1_0": "“Sekhiyan”ti yaṁ vuttaṁ,",
  "pli-tv-pvr10:77.2_0": "taṁ suṇohi yathātathaṁ;",
  "pli-tv-pvr10:77.3_0": "Sekkhassa sikkhamānassa,",
  "pli-tv-pvr10:77.4_0": "ujumaggānusārino.",
  "pli-tv-pvr10:78.1_0": "Ādi cetaṁ caraṇañca,",
  "pli-tv-pvr10:78.2_0": "mukhaṁ saññamasaṁvaro;",
  "pli-tv-pvr10:78.3_0": "Sikkhā etādisī natthi,",
  "pli-tv-pvr10:78.4_0": "tenetaṁ iti vuccati.",
  "pli-tv-pvr10:79.1_0": "Channamativassati,",
  "pli-tv-pvr10:79.2_0": "vivaṭaṁ nātivassati;",
  "pli-tv-pvr10:79.3_0": "Tasmā channaṁ vivaretha,",
  "pli-tv-pvr10:79.4_0": "evaṁ taṁ nātivassati.",
  "pli-tv-pvr10:80.1_0": "Gati migānaṁ pavanaṁ,",
  "pli-tv-pvr10:80.2_0": "Ākāso pakkhinaṁ gati;",
  "pli-tv-pvr10:80.3_0": "Vibhavo gati dhammānaṁ,",
  "pli-tv-pvr10:80.4_0": "Nibbānaṁ arahato gatīti.",
  "pli-tv-pvr10:81.1_0": "Gāthāsaṅgaṇikaṁ niṭṭhitaṁ.",
  "pli-tv-pvr10:82.1_0": "Tassuddānaṁ",
  "pli-tv-pvr10:83.1_0": "Sattanagaresu paññattā,",
  "pli-tv-pvr10:83.2_0": "vipatti caturopi ca;",
  "pli-tv-pvr10:83.3_0": "Bhikkhūnaṁ bhikkhunīnañca,",
  "pli-tv-pvr10:83.4_0": "sādhāraṇā asādhāraṇā;",
  "pli-tv-pvr10:83.5_0": "Sāsanaṁ anuggahāya,",
  "pli-tv-pvr10:83.6_0": "gāthāsaṅgaṇikaṁ idanti.",
  "pli-tv-pvr10:84.1_0": "Gāthāsaṅgaṇikaṁ niṭṭhitaṁ."
}