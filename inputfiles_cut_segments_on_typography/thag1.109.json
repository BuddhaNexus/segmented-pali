{
  "thag1.109:0.1_0": "Theragāthā 1.109",
  "thag1.109:0.2_0": "Ekakanipāta",
  "thag1.109:0.3_0": "Ekādasamavagga",
  "thag1.109:0.4_0": "Saṅgharakkhitattheragāthā",
  "thag1.109:1.1_0": "“Na nūnāyaṁ paramahitānukampino,",
  "thag1.109:1.2_0": "Rahogato anuvigaṇeti sāsanaṁ;",
  "thag1.109:1.3_0": "Tathāhayaṁ viharati pākatindriyo,",
  "thag1.109:1.4_0": "Migī yathā taruṇajātikā vane”ti.",
  "thag1.109:1.5_0": "… Saṅgharakkhito thero …."
}