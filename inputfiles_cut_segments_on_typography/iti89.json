{
  "iti89:0.1_0": "Itivuttaka 89",
  "iti89:0.2_0": "Tikanipāta",
  "iti89:0.3_0": "Catutthavagga",
  "iti89:0.4_0": "Devadattasutta",
  "iti89:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti89:2.1_0": "“Tīhi,",
  "iti89:2.1_1": "bhikkhave,",
  "iti89:2.1_2": "asaddhammehi abhibhūto pariyādinnacitto devadatto āpāyiko nerayiko kappaṭṭho atekiccho.",
  "iti89:2.2_0": "Katamehi tīhi?",
  "iti89:2.3_0": "Pāpicchatāya, bhikkhave, abhibhūto pariyādinnacitto devadatto āpāyiko nerayiko kappaṭṭho atekiccho.",
  "iti89:2.4_0": "Pāpamittatāya, bhikkhave, abhibhūto pariyādinnacitto devadatto āpāyiko nerayiko kappaṭṭho atekiccho.",
  "iti89:2.5_0": "Sati kho pana uttarikaraṇīye oramattakena visesādhigamena antarā vosānaṁ āpādi.",
  "iti89:2.6_0": "Imehi kho,",
  "iti89:2.6_1": "bhikkhave,",
  "iti89:2.6_2": "tīhi asaddhammehi abhibhūto pariyādinnacitto devadatto āpāyiko nerayiko kappaṭṭho atekiccho”ti.",
  "iti89:2.7_0": "Etamatthaṁ bhagavā avoca.",
  "iti89:2.8_0": "Tatthetaṁ iti vuccati:",
  "iti89:3.1_0": "“Mā jātu koci lokasmiṁ,",
  "iti89:3.2_0": "pāpiccho udapajjatha;",
  "iti89:3.3_0": "Tadamināpi jānātha,",
  "iti89:3.4_0": "pāpicchānaṁ yathā gati.",
  "iti89:4.1_0": "Paṇḍitoti samaññāto,",
  "iti89:4.2_0": "bhāvitattoti sammato;",
  "iti89:4.3_0": "Jalaṁva yasasā aṭṭhā,",
  "iti89:4.4_0": "devadattoti vissuto.",
  "iti89:5.1_0": "So samānamanuciṇṇo,",
  "iti89:5.2_0": "āsajja naṁ tathāgataṁ;",
  "iti89:5.3_0": "Avīcinirayaṁ patto,",
  "iti89:5.4_0": "catudvāraṁ bhayānakaṁ.",
  "iti89:6.1_0": "Aduṭṭhassa hi yo dubbhe,",
  "iti89:6.2_0": "pāpakammaṁ akubbato;",
  "iti89:6.3_0": "Tameva pāpaṁ phusati,",
  "iti89:6.4_0": "duṭṭhacittaṁ anādaraṁ.",
  "iti89:7.1_0": "Samuddaṁ visakumbhena,",
  "iti89:7.2_0": "yo maññeyya padūsituṁ;",
  "iti89:7.3_0": "Na so tena padūseyya,",
  "iti89:7.4_0": "bhesmā hi udadhi mahā.",
  "iti89:8.1_0": "Evamevaṁ tathāgataṁ,",
  "iti89:8.2_0": "yo vādena vihiṁsati;",
  "iti89:8.3_0": "Sammaggataṁ santacittaṁ,",
  "iti89:8.4_0": "vādo tamhi na rūhati.",
  "iti89:9.1_0": "Tādisaṁ mittaṁ kubbetha,",
  "iti89:9.2_0": "tañca seveyya paṇḍito;",
  "iti89:9.3_0": "Yassa maggānugo bhikkhu,",
  "iti89:9.4_0": "khayaṁ dukkhassa pāpuṇe”ti.",
  "iti89:10.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti89:10.2_0": "Dasamaṁ.",
  "iti89:10.3_0": "Catuttho vaggo.",
  "iti89:11.0_0": "Tassuddānaṁ",
  "iti89:11.1_0": "Vitakkāsakkārasadda,",
  "iti89:11.2_0": "cavanaloke asubhaṁ;",
  "iti89:11.3_0": "Dhammaandhakāramalaṁ,",
  "iti89:11.4_0": "devadattena te dasāti."
}