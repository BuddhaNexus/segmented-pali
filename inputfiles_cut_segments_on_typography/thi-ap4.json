{
  "thi-ap4:0.1_0": "Therīapadāna",
  "thi-ap4:0.2_0": "Sumedhāvagga",
  "thi-ap4:0.3_0": "4 Saṅkamanatthātherīapadāna",
  "thi-ap4:1.1_0": "“Vipassissa bhagavato,",
  "thi-ap4:1.2_0": "lokajeṭṭhassa tādino;",
  "thi-ap4:1.3_0": "Rathiyaṁ paṭipannassa,",
  "thi-ap4:1.4_0": "tārayantassa pāṇino.",
  "thi-ap4:2.1_0": "Gharato nikkhamitvāna,",
  "thi-ap4:2.2_0": "avakujjā nipajjahaṁ;",
  "thi-ap4:2.3_0": "Anukampako lokanātho,",
  "thi-ap4:2.4_0": "sirasi akkamī mama.",
  "thi-ap4:3.1_0": "Akkamitvāna sirasi,",
  "thi-ap4:3.2_0": "agamā lokanāyako;",
  "thi-ap4:3.3_0": "Tena cittappasādena,",
  "thi-ap4:3.4_0": "tusitaṁ agamāsahaṁ.",
  "thi-ap4:4.1_0": "Kilesā jhāpitā mayhaṁ,",
  "thi-ap4:4.2_0": "…pe…",
  "thi-ap4:4.3_0": "viharāmi anāsavā.",
  "thi-ap4:5.1_0": "Svāgataṁ vata me āsi,",
  "thi-ap4:5.2_0": "…pe…",
  "thi-ap4:5.3_0": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap4:6.1_0": "Paṭisambhidā catasso,",
  "thi-ap4:6.2_0": "…pe…",
  "thi-ap4:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "thi-ap4:7.1_0": "Itthaṁ sudaṁ saṅkamanatthā bhikkhunī imā gāthāyo abhāsitthāti.",
  "thi-ap4:8.1_0": "Saṅkamanatthātheriyāpadānaṁ catutthaṁ."
}