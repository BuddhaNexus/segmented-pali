{
  "mil5.1.9:0.1_0": "Milindapañha",
  "mil5.1.9:0.2_0": "Meṇḍakapañha",
  "mil5.1.9:0.3_0": "Iddhibalavagga",
  "mil5.1.9:0.4_0": "9. Uttarikaraṇīyapañha",
  "mil5.1.9:1.1_0": "“Bhante nāgasena, tumhe bhaṇatha—",
  "mil5.1.9:1.2_0": "‘yaṁ kiñci karaṇīyaṁ tathāgatassa,",
  "mil5.1.9:1.2_1": "sabbaṁ taṁ bodhiyāyeva mūle pariniṭṭhitaṁ,",
  "mil5.1.9:1.2_2": "natthi tathāgatassa uttariṁ karaṇīyaṁ,",
  "mil5.1.9:1.2_3": "katassa vā paticayo’ti,",
  "mil5.1.9:1.2_4": "idañca temāsaṁ paṭisallānaṁ dissati.",
  "mil5.1.9:1.3_0": "Yadi,",
  "mil5.1.9:1.3_1": "bhante nāgasena,",
  "mil5.1.9:1.3_2": "yaṁ kiñci karaṇīyaṁ tathāgatassa,",
  "mil5.1.9:1.3_3": "sabbaṁ taṁ bodhiyāyeva mūle pariniṭṭhitaṁ,",
  "mil5.1.9:1.3_4": "natthi tathāgatassa uttariṁ karaṇīyaṁ,",
  "mil5.1.9:1.3_5": "katassa vā paticayo,",
  "mil5.1.9:1.3_6": "tena hi ‘temāsaṁ paṭisallīno’ti yaṁ vacanaṁ,",
  "mil5.1.9:1.3_7": "taṁ micchā.",
  "mil5.1.9:1.4_0": "Yadi temāsaṁ paṭisallīno,",
  "mil5.1.9:1.4_1": "tena hi ‘yaṁ kiñci karaṇīyaṁ,",
  "mil5.1.9:1.4_2": "tathāgatassa,",
  "mil5.1.9:1.4_3": "sabbaṁ taṁ bodhiyāyeva mūle pariniṭṭhitan’ti tampi vacanaṁ micchā.",
  "mil5.1.9:1.5_0": "Natthi katakaraṇīyassa paṭisallānaṁ,",
  "mil5.1.9:1.5_1": "sakaraṇīyasseva paṭisallānaṁ yathā nāma byādhitasseva bhesajjena karaṇīyaṁ hoti,",
  "mil5.1.9:1.5_2": "abyādhitassa kiṁ bhesajjena.",
  "mil5.1.9:1.6_0": "Chātasseva bhojanena karaṇīyaṁ hoti, achātassa kiṁ bhojanena.",
  "mil5.1.9:1.7_0": "Evameva kho, bhante nāgasena, natthi katakaraṇīyassa paṭisallānaṁ, sakaraṇīyasseva paṭisallānaṁ.",
  "mil5.1.9:1.8_0": "Ayampi ubhato koṭiko pañho tavānuppatto, so tayā nibbāhitabbo”ti.",
  "mil5.1.9:2.1_0": "“Yaṁ kiñci, mahārāja,",
  "mil5.1.9:2.1_1": "karaṇīyaṁ tathāgatassa,",
  "mil5.1.9:2.1_2": "sabbaṁ taṁ bodhiyāyeva mūle pariniṭṭhitaṁ,",
  "mil5.1.9:2.1_3": "natthi tathāgatassa uttariṁ karaṇīyaṁ,",
  "mil5.1.9:2.1_4": "katassa vā paticayo,",
  "mil5.1.9:2.1_5": "bhagavā ca temāsaṁ paṭisallīno,",
  "mil5.1.9:2.1_6": "paṭisallānaṁ kho, mahārāja,",
  "mil5.1.9:2.1_7": "bahuguṇaṁ,",
  "mil5.1.9:2.1_8": "sabbepi tathāgatā paṭisallīyitvā sabbaññutaṁ pattā,",
  "mil5.1.9:2.1_9": "taṁ te sukataguṇamanussarantā paṭisallānaṁ sevanti.",
  "mil5.1.9:2.2_0": "Yathā, mahārāja,",
  "mil5.1.9:2.2_1": "puriso rañño santikā laddhavaro paṭiladdhabhogo taṁ sukataguṇamanussaranto aparāparaṁ rañño upaṭṭhānaṁ eti;",
  "mil5.1.9:2.3_0": "evameva kho, mahārāja,",
  "mil5.1.9:2.3_1": "sabbepi tathāgatā paṭisallīyitvā sabbaññutaṁ pattā,",
  "mil5.1.9:2.3_2": "taṁ te sukataguṇamanussarantā paṭisallānaṁ sevanti.",
  "mil5.1.9:3.1_0": "Yathā vā pana, mahārāja,",
  "mil5.1.9:3.1_1": "puriso āturo dukkhito bāḷhagilāno bhisakkamupasevitvā sotthimanuppatto taṁ sukataguṇamanussaranto aparāparaṁ bhisakkamupasevati;",
  "mil5.1.9:3.2_0": "evameva kho, mahārāja,",
  "mil5.1.9:3.2_1": "sabbepi tathāgatā paṭisallīyitvā sabbaññutaṁ pattā,",
  "mil5.1.9:3.2_2": "taṁ te sukataguṇamanussarantā paṭisallānaṁ sevanti.",
  "mil5.1.9:4.1_0": "Aṭṭhavīsati kho panime, mahārāja,",
  "mil5.1.9:4.1_1": "paṭisallānaguṇā,",
  "mil5.1.9:4.1_2": "ye guṇe samanussarantā tathāgatā paṭisallānaṁ sevanti.",
  "mil5.1.9:4.2_0": "Katame aṭṭhavīsati?",
  "mil5.1.9:4.3_0": "Idha, mahārāja,",
  "mil5.1.9:4.3_1": "paṭisallānaṁ paṭisallīyamānaṁ attānaṁ rakkhati,",
  "mil5.1.9:4.3_2": "āyuṁ vaḍḍheti,",
  "mil5.1.9:4.3_3": "balaṁ deti,",
  "mil5.1.9:4.3_4": "vajjaṁ pidahati,",
  "mil5.1.9:4.3_5": "ayasamapaneti,",
  "mil5.1.9:4.3_6": "yasamupaneti,",
  "mil5.1.9:4.3_7": "aratiṁ vinodeti,",
  "mil5.1.9:4.3_8": "ratimupadahati,",
  "mil5.1.9:4.3_9": "bhayamapaneti,",
  "mil5.1.9:4.3_10": "vesārajjaṁ karoti,",
  "mil5.1.9:4.3_11": "kosajjamapaneti,",
  "mil5.1.9:4.3_12": "vīriyamabhijaneti,",
  "mil5.1.9:4.3_13": "rāgamapaneti,",
  "mil5.1.9:4.3_14": "dosamapaneti,",
  "mil5.1.9:4.3_15": "mohamapaneti,",
  "mil5.1.9:4.3_16": "mānaṁ nihanti,",
  "mil5.1.9:4.3_17": "vitakkaṁ bhañjati,",
  "mil5.1.9:4.3_18": "cittaṁ ekaggaṁ karoti,",
  "mil5.1.9:4.3_19": "mānasaṁ snehayati,",
  "mil5.1.9:4.3_20": "hāsaṁ janeti,",
  "mil5.1.9:4.3_21": "garukaṁ karoti,",
  "mil5.1.9:4.3_22": "lābhamuppādayati,",
  "mil5.1.9:4.3_23": "namassiyaṁ karoti,",
  "mil5.1.9:4.3_24": "pītiṁ pāpeti,",
  "mil5.1.9:4.3_25": "pāmojjaṁ karoti,",
  "mil5.1.9:4.3_26": "saṅkhārānaṁ sabhāvaṁ dassayati,",
  "mil5.1.9:4.3_27": "bhavappaṭisandhiṁ ugghāṭeti,",
  "mil5.1.9:4.3_28": "sabbasāmaññaṁ deti.",
  "mil5.1.9:4.4_0": "Ime kho, mahārāja,",
  "mil5.1.9:4.4_1": "aṭṭhavīsati paṭisallānaguṇā,",
  "mil5.1.9:4.4_2": "ye guṇe samanussarantā tathāgatā paṭisallānaṁ sevanti.",
  "mil5.1.9:5.1_0": "Api ca kho, mahārāja,",
  "mil5.1.9:5.1_1": "tathāgatā santaṁ sukhaṁ samāpattiratiṁ anubhavitukāmā paṭisallānaṁ sevanti pariyositasaṅkappā.",
  "mil5.1.9:5.2_0": "Catūhi kho, mahārāja, kāraṇehi tathāgatā paṭisallānaṁ sevanti.",
  "mil5.1.9:5.3_0": "Katamehi catūhi?",
  "mil5.1.9:5.4_0": "Vihāraphāsutāyapi, mahārāja,",
  "mil5.1.9:5.4_1": "tathāgatā paṭisallānaṁ sevanti,",
  "mil5.1.9:5.4_2": "anavajjaguṇabahulatāyapi tathāgatā paṭisallānaṁ sevanti,",
  "mil5.1.9:5.4_3": "asesaariyavīthitopi tathāgatā paṭisallānaṁ sevanti,",
  "mil5.1.9:5.4_4": "sabbabuddhānaṁ thutathomitavaṇṇitapasatthatopi tathāgatā paṭisallānaṁ sevanti.",
  "mil5.1.9:5.5_0": "Imehi kho, mahārāja, catūhi kāraṇehi tathāgatā paṭisallānaṁ sevanti.",
  "mil5.1.9:5.6_0": "Iti kho, mahārāja,",
  "mil5.1.9:5.6_1": "tathāgatā paṭisallānaṁ sevanti na sakaraṇīyatāya,",
  "mil5.1.9:5.6_2": "na katassa vā paticayāya,",
  "mil5.1.9:5.6_3": "atha kho guṇavisesadassāvitāya tathāgatā paṭisallānaṁ sevantī”ti.",
  "mil5.1.9:5.7_0": "“Sādhu, bhante nāgasena, evametaṁ tathā sampaṭicchāmī”ti.",
  "mil5.1.9:6.1_0": "Uttarikaraṇīyapañho navamo."
}