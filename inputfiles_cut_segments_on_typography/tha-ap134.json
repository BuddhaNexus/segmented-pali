{
  "tha-ap134:0.1_0": "Therāpadāna",
  "tha-ap134:0.2_0": "Sobhitavagga",
  "tha-ap134:0.3_0": "2. Sudassanattheraapadāna",
  "tha-ap134:1.1_0": "“Vinatā nadiyā tīre,",
  "tha-ap134:1.2_0": "pilakkhu phalito ahu;",
  "tha-ap134:1.3_0": "Tāhaṁ rukkhaṁ gavesanto,",
  "tha-ap134:1.4_0": "addasaṁ lokanāyakaṁ.",
  "tha-ap134:2.1_0": "Ketakaṁ pupphitaṁ disvā,",
  "tha-ap134:2.2_0": "vaṇṭe chetvānahaṁ tadā;",
  "tha-ap134:2.3_0": "Buddhassa abhiropesiṁ,",
  "tha-ap134:2.4_0": "sikhino lokabandhuno.",
  "tha-ap134:3.1_0": "Yena ñāṇena pattosi,",
  "tha-ap134:3.2_0": "accutaṁ amataṁ padaṁ;",
  "tha-ap134:3.3_0": "Taṁ ñāṇaṁ abhipūjemi,",
  "tha-ap134:3.4_0": "buddhaseṭṭha mahāmuni.",
  "tha-ap134:4.1_0": "Ñāṇamhi pūjaṁ katvāna,",
  "tha-ap134:4.2_0": "pilakkhumaddasaṁ ahaṁ;",
  "tha-ap134:4.3_0": "Paṭiladdhomhi taṁ paññaṁ,",
  "tha-ap134:4.4_0": "ñāṇapūjāyidaṁ phalaṁ.",
  "tha-ap134:5.1_0": "Ekattiṁse ito kappe,",
  "tha-ap134:5.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap134:5.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap134:5.4_0": "ñāṇapūjāyidaṁ phalaṁ.",
  "tha-ap134:6.1_0": "Ito terasakappamhi,",
  "tha-ap134:6.2_0": "dvādasāsuṁ phaluggatā;",
  "tha-ap134:6.3_0": "Sattaratanasampannā,",
  "tha-ap134:6.4_0": "cakkavattī mahapphalā.",
  "tha-ap134:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap134:7.2_0": "…pe…",
  "tha-ap134:7.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap134:8.1_0": "Itthaṁ sudaṁ āyasmā sudassano thero imā gāthāyo abhāsitthāti.",
  "tha-ap134:9.1_0": "Sudassanattherassāpadānaṁ dutiyaṁ."
}