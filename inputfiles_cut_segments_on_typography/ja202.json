{
  "ja202:0.1_0": "Jātaka",
  "ja202:0.2_0": "Dukanipāta",
  "ja202:0.3_0": "Nataṁdaḷhavagga",
  "ja202:0.4_0": "2. Keḷisīlajātaka",
  "ja202:1.1_0": "“Haṁsā koñcā mayūrā ca,",
  "ja202:1.2_0": "hatthayo pasadā migā;",
  "ja202:1.3_0": "Sabbe sīhassa bhāyanti,",
  "ja202:1.4_0": "natthi kāyasmi tulyatā.",
  "ja202:2.1_0": "Evameva manussesu,",
  "ja202:2.2_0": "daharo cepi paññavā;",
  "ja202:2.3_0": "So hi tattha mahā hoti,",
  "ja202:2.4_0": "neva bālo sarīravā”ti.",
  "ja202:3.1_0": "Keḷisīlajātakaṁ dutiyaṁ."
}