{
  "ja251:0.1_0": "Jātaka",
  "ja251:0.2_0": "Tikanipāta",
  "ja251:0.3_0": "Saṅkappavagga",
  "ja251:0.4_0": "1. Saṅkapparāgajātaka",
  "ja251:1.1_0": "“Saṅkapparāgadhotena,",
  "ja251:1.2_0": "vitakkanisitena ca;",
  "ja251:1.3_0": "Nālaṅkatena bhadrena,",
  "ja251:1.4_0": "usukārākatena ca.",
  "ja251:2.1_0": "Na kaṇṇāyatamuttena,",
  "ja251:2.2_0": "nāpi morūpasevinā;",
  "ja251:2.3_0": "Tenamhi hadaye viddho,",
  "ja251:2.4_0": "sabbaṅgaparidāhinā.",
  "ja251:3.1_0": "Āvedhañca na passāmi,",
  "ja251:3.2_0": "Yato ruhiramassave;",
  "ja251:3.3_0": "Yāva ayoniso cittaṁ,",
  "ja251:3.4_0": "Sayaṁ me dukkhamābhatan”ti.",
  "ja251:4.1_0": "Saṅkapparāgajātakaṁ paṭhamaṁ."
}