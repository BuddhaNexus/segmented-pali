{
  "tha-ap306:0.1_0": "Therāpadāna",
  "tha-ap306:0.2_0": "Padumakesaravagga",
  "tha-ap306:0.3_0": "4. Dhammasaññakattheraapadāna",
  "tha-ap306:1.1_0": "“Vipassino bhagavato,",
  "tha-ap306:1.2_0": "Mahābodhimaho ahu;",
  "tha-ap306:1.3_0": "Rukkhaṭṭhasseva sambuddho,",
  "tha-ap306:1.4_0": "Lokajeṭṭho narāsabho.",
  "tha-ap306:2.1_0": "Bhagavā tamhi samaye,",
  "tha-ap306:2.2_0": "bhikkhusaṅghapurakkhato;",
  "tha-ap306:2.3_0": "Catusaccaṁ pakāseti,",
  "tha-ap306:2.4_0": "vācāsabhimudīrayaṁ.",
  "tha-ap306:3.1_0": "Saṅkhittena ca desento,",
  "tha-ap306:3.2_0": "vitthārena ca desayaṁ;",
  "tha-ap306:3.3_0": "Vivaṭṭacchado sambuddho,",
  "tha-ap306:3.4_0": "nibbāpesi mahājanaṁ.",
  "tha-ap306:4.1_0": "Tassāhaṁ dhammaṁ sutvāna,",
  "tha-ap306:4.2_0": "lokajeṭṭhassa tādino;",
  "tha-ap306:4.3_0": "Vanditvā satthuno pāde,",
  "tha-ap306:4.4_0": "pakkāmiṁ uttarāmukho.",
  "tha-ap306:5.1_0": "Ekanavutito kappe,",
  "tha-ap306:5.2_0": "yaṁ dhammamasuṇiṁ tadā;",
  "tha-ap306:5.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap306:5.4_0": "dhammasavassidaṁ phalaṁ.",
  "tha-ap306:6.1_0": "Tettiṁsamhi ito kappe,",
  "tha-ap306:6.2_0": "eko āsiṁ mahīpati;",
  "tha-ap306:6.3_0": "Sutavā nāma nāmena,",
  "tha-ap306:6.4_0": "cakkavattī mahabbalo.",
  "tha-ap306:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap306:7.2_0": "…pe…",
  "tha-ap306:7.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap306:8.1_0": "Itthaṁ sudaṁ āyasmā dhammasaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap306:9.1_0": "Dhammasaññakattherassāpadānaṁ catutthaṁ."
}