{
  "thag1.69:0.1_0": "Theragāthā 1.69",
  "thag1.69:0.2_0": "Ekakanipāta",
  "thag1.69:0.3_0": "Sattamavagga",
  "thag1.69:0.4_0": "Channattheragāthā",
  "thag1.69:1.1_0": "“Sutvāna dhammaṁ mahato mahārasaṁ,",
  "thag1.69:1.2_0": "Sabbaññutaññāṇavarena desitaṁ;",
  "thag1.69:1.3_0": "Maggaṁ papajjiṁ amatassa pattiyā,",
  "thag1.69:1.4_0": "So yogakkhemassa pathassa kovido”ti.",
  "thag1.69:1.5_0": "… Channo thero …."
}