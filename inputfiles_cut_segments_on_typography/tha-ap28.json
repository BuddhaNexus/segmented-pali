{
  "tha-ap28:0.1_0": "Therāpadāna",
  "tha-ap28:0.2_0": "Subhūtivagga",
  "tha-ap28:0.3_0": "6. Dhūpadāyakattheraapadāna",
  "tha-ap28:1.1_0": "“Siddhatthassa bhagavato,",
  "tha-ap28:1.2_0": "lokajeṭṭhassa tādino;",
  "tha-ap28:1.3_0": "Kuṭidhūpaṁ mayā dinnaṁ,",
  "tha-ap28:1.4_0": "vippasannena cetasā.",
  "tha-ap28:2.1_0": "Yaṁ yaṁ yonupapajjāmi,",
  "tha-ap28:2.2_0": "devattaṁ atha mānusaṁ;",
  "tha-ap28:2.3_0": "Sabbesampi piyo homi,",
  "tha-ap28:2.4_0": "dhūpadānassidaṁ phalaṁ.",
  "tha-ap28:3.1_0": "Catunnavutito kappe,",
  "tha-ap28:3.2_0": "yaṁ dhūpamadadiṁ tadā;",
  "tha-ap28:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap28:3.4_0": "dhūpadānassidaṁ phalaṁ.",
  "tha-ap28:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap28:4.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap28:4.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap28:4.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap28:5.1_0": "Itthaṁ sudaṁ āyasmā dhūpadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap28:6.1_0": "Dhūpadāyakattherassāpadānaṁ chaṭṭhaṁ."
}