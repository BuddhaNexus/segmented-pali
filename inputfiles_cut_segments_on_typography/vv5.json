{
  "vv5:0.1_0": "Vimānavatthu",
  "vv5:0.2_0": "Itthivimāna",
  "vv5:0.3_0": "Pīṭhavagga",
  "vv5:0.4_0": "5. Kuñjaravimānavatthu",
  "vv5:1.1_0": "“Kuñjaro te varāroho,",
  "vv5:1.2_0": "Nānāratanakappano;",
  "vv5:1.3_0": "Ruciro thāmavā javasampanno,",
  "vv5:1.4_0": "Ākāsamhi samīhati.",
  "vv5:2.1_0": "Padumi padmapattakkhi,",
  "vv5:2.2_0": "padmuppalajutindharo;",
  "vv5:2.3_0": "Padmacuṇṇābhikiṇṇaṅgo,",
  "vv5:2.4_0": "soṇṇapokkharamāladhā.",
  "vv5:3.1_0": "Padumānusaṭaṁ maggaṁ,",
  "vv5:3.2_0": "padmapattavibhūsitaṁ;",
  "vv5:3.3_0": "Ṭhitaṁ vaggu manugghātī,",
  "vv5:3.4_0": "mitaṁ gacchati vāraṇo.",
  "vv5:4.1_0": "Tassa pakkamamānassa,",
  "vv5:4.2_0": "soṇṇakaṁsā ratissarā;",
  "vv5:4.3_0": "Tesaṁ suyyati nigghoso,",
  "vv5:4.4_0": "tūriye pañcaṅgike yathā.",
  "vv5:5.1_0": "Tassa nāgassa khandhamhi,",
  "vv5:5.2_0": "sucivatthā alaṅkatā;",
  "vv5:5.3_0": "Mahantaṁ accharāsaṅghaṁ,",
  "vv5:5.4_0": "vaṇṇena atirocasi.",
  "vv5:6.1_0": "Dānassa te idaṁ phalaṁ,",
  "vv5:6.2_0": "atho sīlassa vā pana;",
  "vv5:6.3_0": "Atho añjalikammassa,",
  "vv5:6.4_0": "taṁ me akkhāhi pucchitā”ti.",
  "vv5:7.1_0": "Sā devatā attamanā,",
  "vv5:7.2_0": "moggallānena pucchitā;",
  "vv5:7.3_0": "Pañhaṁ puṭṭhā viyākāsi,",
  "vv5:7.4_0": "yassa kammassidaṁ phalaṁ.",
  "vv5:8.1_0": "“Disvāna guṇasampannaṁ,",
  "vv5:8.2_0": "jhāyiṁ jhānarataṁ sataṁ;",
  "vv5:8.3_0": "Adāsiṁ pupphābhikiṇṇaṁ,",
  "vv5:8.4_0": "āsanaṁ dussasanthataṁ.",
  "vv5:9.1_0": "Upaḍḍhaṁ padmamālāhaṁ,",
  "vv5:9.2_0": "āsanassa samantato;",
  "vv5:9.3_0": "Abbhokirissaṁ pattehi,",
  "vv5:9.4_0": "pasannā sehi pāṇibhi.",
  "vv5:10.1_0": "Tassa kammakusalassa,",
  "vv5:10.2_0": "idaṁ me īdisaṁ phalaṁ;",
  "vv5:10.3_0": "Sakkāro garukāro ca,",
  "vv5:10.4_0": "devānaṁ apacitā ahaṁ.",
  "vv5:11.1_0": "Yo ve sammāvimuttānaṁ,",
  "vv5:11.2_0": "santānaṁ brahmacārinaṁ;",
  "vv5:11.3_0": "Pasanno āsanaṁ dajjā,",
  "vv5:11.4_0": "evaṁ nande yathā ahaṁ.",
  "vv5:12.1_0": "Tasmā hi attakāmena,",
  "vv5:12.2_0": "mahattamabhikaṅkhatā;",
  "vv5:12.3_0": "Āsanaṁ dātabbaṁ hoti,",
  "vv5:12.4_0": "sarīrantimadhārinan”ti.",
  "vv5:13.1_0": "Kuñjaravimānaṁ pañcamaṁ."
}