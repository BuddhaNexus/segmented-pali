{
  "pp2.10:0.1_0": "Puggalapaññatti",
  "pp2.10:0.2_0": "Niddesa",
  "pp2.10:0.3_0": "10. Dasakapuggalapaññatti",
  "pp2.10:1.1_0": "Katamesaṁ pañcannaṁ idha niṭṭhā?",
  "pp2.10:1.2_0": "Sattakkhattuparamassa, kolaṅkolassa, ekabījissa, sakadāgāmissa, yo ca diṭṭheva dhamme arahā—",
  "pp2.10:1.3_0": "imesaṁ pañcannaṁ idha niṭṭhā. –5",
  "pp2.10:2.1_0": "Katamesaṁ pañcannaṁ idha vihāya niṭṭhā?",
  "pp2.10:2.2_0": "Antarāparinibbāyissa,",
  "pp2.10:2.2_1": "upahaccaparinibbāyissa,",
  "pp2.10:2.2_2": "asaṅkhāraparinibbāyissa,",
  "pp2.10:2.2_3": "sasaṅkhāraparinibbāyissa,",
  "pp2.10:2.2_4": "uddhaṁsotassa akaniṭṭhagāmino—",
  "pp2.10:2.3_0": "imesaṁ pañcannaṁ idha vihāya niṭṭhāti. –10",
  "pp2.10:3.1_0": "Ettāvatā puggalānaṁ puggalapaññattīti.",
  "pp2.10:4.1_0": "Dasakaniddeso.",
  "pp2.10:5.1_0": "Puggalapaññattipakaraṇaṁ niṭṭhitaṁ."
}