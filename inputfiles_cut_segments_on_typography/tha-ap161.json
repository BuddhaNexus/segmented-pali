{
  "tha-ap161:0.1_0": "Therāpadāna",
  "tha-ap161:0.2_0": "Bandhujīvakavagga",
  "tha-ap161:0.3_0": "9. Punnāgapupphiyattheraapadāna",
  "tha-ap161:1.1_0": "“Kānanaṁ vanamogayha,",
  "tha-ap161:1.2_0": "vasāmi luddako ahaṁ;",
  "tha-ap161:1.3_0": "Punnāgaṁ pupphitaṁ disvā,",
  "tha-ap161:1.4_0": "buddhaseṭṭhaṁ anussariṁ.",
  "tha-ap161:2.1_0": "Taṁ pupphaṁ ocinitvāna,",
  "tha-ap161:2.2_0": "sugandhaṁ gandhitaṁ subhaṁ;",
  "tha-ap161:2.3_0": "Thūpaṁ karitvā puline,",
  "tha-ap161:2.4_0": "buddhassa abhiropayiṁ.",
  "tha-ap161:3.1_0": "Dvenavute ito kappe,",
  "tha-ap161:3.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap161:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap161:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap161:4.1_0": "Ekamhi navute kappe,",
  "tha-ap161:4.2_0": "eko āsiṁ tamonudo;",
  "tha-ap161:4.3_0": "Sattaratanasampanno,",
  "tha-ap161:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap161:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap161:5.2_0": "…pe…",
  "tha-ap161:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap161:6.1_0": "Itthaṁ sudaṁ āyasmā punnāgapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap161:7.1_0": "Punnāgapupphiyattherassāpadānaṁ navamaṁ."
}