{
  "pli-tv-bi-vb-pc96:0.1_0": "Theravāda Vinaya",
  "pli-tv-bi-vb-pc96:0.2_0": "Bhikkhunivibhaṅga",
  "pli-tv-bi-vb-pc96:0.3_0": "Pācittiyakaṇḍa",
  "pli-tv-bi-vb-pc96:0.4_0": "Chattupāhanavagga",
  "pli-tv-bi-vb-pc96:0.5_0": "96. Asaṅkaccikasikkhāpada",
  "pli-tv-bi-vb-pc96:1.1_0": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bi-vb-pc96:1.2_0": "Tena kho pana samayena aññatarā bhikkhunī asaṅkaccikā gāmaṁ piṇḍāya pāvisi.",
  "pli-tv-bi-vb-pc96:1.3_0": "Tassā rathikāya vātamaṇḍalikā saṅghāṭiyo ukkhipiṁsu.",
  "pli-tv-bi-vb-pc96:1.4_0": "Manussā ukkuṭṭhiṁ akaṁsu—",
  "pli-tv-bi-vb-pc96:1.5_0": "“sundarā ayyāya thanudarā”ti.",
  "pli-tv-bi-vb-pc96:1.6_0": "Sā bhikkhunī tehi manussehi uppaṇḍiyamānā maṅku ahosi.",
  "pli-tv-bi-vb-pc96:1.7_0": "Atha kho sā bhikkhunī upassayaṁ gantvā bhikkhunīnaṁ etamatthaṁ ārocesi.",
  "pli-tv-bi-vb-pc96:1.8_0": "Yā tā bhikkhuniyo appicchā …pe… tā ujjhāyanti khiyyanti vipācenti—",
  "pli-tv-bi-vb-pc96:1.9_0": "“kathañhi nāma bhikkhunī asaṅkaccikā gāmaṁ pavisissatī”ti …pe…",
  "pli-tv-bi-vb-pc96:1.10_0": "“saccaṁ kira, bhikkhave, bhikkhunī asaṅkaccikā gāmaṁ pāvisī”ti?",
  "pli-tv-bi-vb-pc96:1.11_0": "“Saccaṁ, bhagavā”ti.",
  "pli-tv-bi-vb-pc96:1.12_0": "Vigarahi buddho bhagavā …pe…",
  "pli-tv-bi-vb-pc96:1.13_0": "kathañhi nāma, bhikkhave, bhikkhunī asaṅkaccikā gāmaṁ pavisissati.",
  "pli-tv-bi-vb-pc96:1.14_0": "Netaṁ, bhikkhave, appasannānaṁ vā pasādāya …pe…",
  "pli-tv-bi-vb-pc96:1.15_0": "evañca pana, bhikkhave, bhikkhuniyo imaṁ sikkhāpadaṁ uddisantu—",
  "pli-tv-bi-vb-pc96:1.16.1_0": "“Yā pana bhikkhunī asaṅkaccikā gāmaṁ paviseyya, pācittiyan”ti.",
  "pli-tv-bi-vb-pc96:2.1.1_0": "Yā panāti",
  "pli-tv-bi-vb-pc96:2.1.2_0": "yā yādisā …pe…",
  "pli-tv-bi-vb-pc96:2.1.3_0": "bhikkhunīti",
  "pli-tv-bi-vb-pc96:2.1.4_0": "…pe… ayaṁ imasmiṁ atthe adhippetā bhikkhunīti.",
  "pli-tv-bi-vb-pc96:2.1.5_0": "Asaṅkaccikāti",
  "pli-tv-bi-vb-pc96:2.1.6_0": "vinā saṅkaccikaṁ.",
  "pli-tv-bi-vb-pc96:2.1.7_0": "Saṅkaccikaṁ nāma",
  "pli-tv-bi-vb-pc96:2.1.8_0": "adhakkhakaṁ ubbhanābhi, tassa paṭicchādanatthāya.",
  "pli-tv-bi-vb-pc96:2.1.9_0": "Gāmaṁ paviseyyāti",
  "pli-tv-bi-vb-pc96:2.1.10_0": "parikkhittassa gāmassa parikkhepaṁ atikkāmentiyā āpatti pācittiyassa.",
  "pli-tv-bi-vb-pc96:2.1.11_0": "Aparikkhittassa gāmassa upacāraṁ okkamantiyā āpatti pācittiyassa.",
  "pli-tv-bi-vb-pc96:2.2.1_0": "Anāpatti—",
  "pli-tv-bi-vb-pc96:2.2.2_0": "acchinnacīvarikāya,",
  "pli-tv-bi-vb-pc96:2.2.3_0": "naṭṭhacīvarikāya,",
  "pli-tv-bi-vb-pc96:2.2.4_0": "gilānāya,",
  "pli-tv-bi-vb-pc96:2.2.5_0": "assatiyā,",
  "pli-tv-bi-vb-pc96:2.2.6_0": "ajānantiyā,",
  "pli-tv-bi-vb-pc96:2.2.7_0": "āpadāsu,",
  "pli-tv-bi-vb-pc96:2.2.8_0": "ummattikāya,",
  "pli-tv-bi-vb-pc96:2.2.9_0": "ādikammikāyāti.",
  "pli-tv-bi-vb-pc96:2.2.10_0": "Terasamasikkhāpadaṁ niṭṭhitaṁ.",
  "pli-tv-bi-vb-pc96:2.2.11_0": "Chattupāhanavaggo navamo.",
  "pli-tv-bi-vb-pc96:2.2.12.0_0": "Dhammikavagga",
  "pli-tv-bi-vb-pc96:2.2.13.0_0": "Nandasikkhāpada",
  "pli-tv-bi-vb-pc96:2.2.14_0": "Uddiṭṭhā kho, ayyāyo, chasaṭṭhisatā pācittiyā dhammā.",
  "pli-tv-bi-vb-pc96:2.2.15_0": "Tatthāyyāyo pucchāmi—",
  "pli-tv-bi-vb-pc96:2.2.16_0": "“kaccittha parisuddhā”?",
  "pli-tv-bi-vb-pc96:2.2.17_0": "Dutiyampi pucchāmi—",
  "pli-tv-bi-vb-pc96:2.2.18_0": "“kaccittha parisuddhā”?",
  "pli-tv-bi-vb-pc96:2.2.19_0": "Tatiyampi pucchāmi—",
  "pli-tv-bi-vb-pc96:2.2.20_0": "“kaccittha parisuddhā”?",
  "pli-tv-bi-vb-pc96:2.2.21_0": "Parisuddhetthāyyāyo, tasmā tuṇhī, evametaṁ dhārayāmīti.",
  "pli-tv-bi-vb-pc96:2.2.22_0": "Khuddakaṁ samattaṁ.",
  "pli-tv-bi-vb-pc96:2.2.23_0": "Bhikkhunivibhaṅge pācittiyakaṇḍaṁ niṭṭhitaṁ.",
  "pli-tv-bi-vb-pc96:2.2.24_0": "(…)"
}