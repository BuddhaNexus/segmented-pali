{
  "kp4:0.1_0": "Khuddakapāṭha 4",
  "kp4:0.2_0": "Kumārapañhā",
  "kp4:1.1_0": "Ekaṁ nāma kiṁ?",
  "kp4:1.2_0": "Sabbe sattā āhāraṭṭhitikā.",
  "kp4:2.1_0": "Dve nāma kiṁ?",
  "kp4:2.2_0": "Nāmañca rūpañca.",
  "kp4:3.1_0": "Tīṇi nāma kiṁ?",
  "kp4:3.2_0": "Tisso vedanā.",
  "kp4:4.1_0": "Cattāri nāma kiṁ?",
  "kp4:4.2_0": "Cattāri ariyasaccāni.",
  "kp4:5.1_0": "Pañca nāma kiṁ?",
  "kp4:5.2_0": "Pañcupādānakkhandhā.",
  "kp4:6.1_0": "Cha nāma kiṁ?",
  "kp4:6.2_0": "Cha ajjhattikāni āyatanāni.",
  "kp4:7.1_0": "Satta nāma kiṁ?",
  "kp4:7.2_0": "Satta bojjhaṅgā.",
  "kp4:8.1_0": "Aṭṭha nāma kiṁ?",
  "kp4:8.2_0": "Ariyo aṭṭhaṅgiko maggo.",
  "kp4:9.1_0": "Nava nāma kiṁ?",
  "kp4:9.2_0": "Nava sattāvāsā.",
  "kp4:10.1_0": "Dasa nāma kiṁ?",
  "kp4:10.2_0": "Dasahaṅgehi samannāgato “arahā”ti vuccatīti.",
  "kp4:11.1_0": "Kumārapañhā."
}