{
  "tha-ap516:0.1_0": "Therāpadāna",
  "tha-ap516:0.2_0": "Phaladāyakavagga",
  "tha-ap516:0.3_0": "4. Ketakapupphiyattheraapadāna",
  "tha-ap516:1.1_0": "“Vinatānadiyā tīre,",
  "tha-ap516:1.2_0": "vihāsi purisuttamo;",
  "tha-ap516:1.3_0": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap516:1.4_0": "ekaggaṁ susamāhitaṁ.",
  "tha-ap516:2.1_0": "Madhugandhassa pupphena,",
  "tha-ap516:2.2_0": "ketakassa ahaṁ tadā;",
  "tha-ap516:2.3_0": "Pasannacitto sumano,",
  "tha-ap516:2.4_0": "buddhaseṭṭhamapūjayiṁ.",
  "tha-ap516:3.1_0": "Ekanavutito kappe,",
  "tha-ap516:3.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap516:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap516:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap516:4.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap516:4.2_0": "…pe…",
  "tha-ap516:4.3_0": "viharāmi anāsavo.",
  "tha-ap516:5.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap516:5.2_0": "…pe…",
  "tha-ap516:5.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap516:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap516:6.2_0": "…pe…",
  "tha-ap516:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap516:7.1_0": "Itthaṁ sudaṁ āyasmā ketakapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap516:8.1_0": "Ketakapupphiyattherassāpadānaṁ catutthaṁ."
}