{
  "tha-ap342:0.1_0": "Therāpadāna",
  "tha-ap342:0.2_0": "Gandhodakavagga",
  "tha-ap342:0.3_0": "10. Ukkhittapadumiyattheraapadāna",
  "tha-ap342:1.1_0": "“Nagare haṁsavatiyā,",
  "tha-ap342:1.2_0": "ahosiṁ māliko tadā;",
  "tha-ap342:1.3_0": "Ogāhetvā padumasaraṁ,",
  "tha-ap342:1.4_0": "satapattaṁ ocināmahaṁ.",
  "tha-ap342:2.1_0": "Padumuttaro nāma jino,",
  "tha-ap342:2.2_0": "sabbadhammāna pāragū;",
  "tha-ap342:2.3_0": "Saha satasahassehi,",
  "tha-ap342:2.4_0": "santacittehi tādibhi.",
  "tha-ap342:3.1_0": "Khīṇāsavehi suddhehi,",
  "tha-ap342:3.2_0": "chaḷabhiññehi jhāyibhi;",
  "tha-ap342:3.3_0": "Mama vuddhiṁ samanvesaṁ,",
  "tha-ap342:3.4_0": "āgacchi mama santikaṁ.",
  "tha-ap342:4.1_0": "Disvānahaṁ devadevaṁ,",
  "tha-ap342:4.2_0": "sayambhuṁ lokanāyakaṁ;",
  "tha-ap342:4.3_0": "Vaṇṭe chetvā satapattaṁ,",
  "tha-ap342:4.4_0": "ukkhipimambare tadā.",
  "tha-ap342:5.1_0": "Yadi buddho tuvaṁ vīra,",
  "tha-ap342:5.2_0": "lokajeṭṭho narāsabho;",
  "tha-ap342:5.3_0": "Sayaṁ gantvā satapattā,",
  "tha-ap342:5.4_0": "matthake dhārayantu te.",
  "tha-ap342:6.1_0": "Adhiṭṭhahi mahāvīro,",
  "tha-ap342:6.2_0": "lokajeṭṭho narāsabho;",
  "tha-ap342:6.3_0": "Buddhassa ānubhāvena,",
  "tha-ap342:6.4_0": "matthake dhārayiṁsu te.",
  "tha-ap342:7.1_0": "Tena kammena sukatena,",
  "tha-ap342:7.2_0": "cetanāpaṇidhīhi ca;",
  "tha-ap342:7.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "tha-ap342:7.4_0": "tāvatiṁsamagacchahaṁ.",
  "tha-ap342:8.1_0": "Tattha me sukataṁ byamhaṁ,",
  "tha-ap342:8.2_0": "satapattanti vuccati;",
  "tha-ap342:8.3_0": "Saṭṭhiyojanamubbiddhaṁ,",
  "tha-ap342:8.4_0": "tiṁsayojanavitthataṁ.",
  "tha-ap342:9.1_0": "Sahassakkhattuṁ devindo,",
  "tha-ap342:9.2_0": "devarajjamakārayiṁ;",
  "tha-ap342:9.3_0": "Pañcasattatikkhattuñca,",
  "tha-ap342:9.4_0": "cakkavattī ahosahaṁ.",
  "tha-ap342:10.1_0": "Padesarajjaṁ vipulaṁ,",
  "tha-ap342:10.2_0": "gaṇanāto asaṅkhiyaṁ;",
  "tha-ap342:10.3_0": "Anubhomi sakaṁ kammaṁ,",
  "tha-ap342:10.4_0": "pubbe sukatamattano.",
  "tha-ap342:11.1_0": "Tenevekapadumena,",
  "tha-ap342:11.2_0": "anubhotvāna sampadā;",
  "tha-ap342:11.3_0": "Gotamassa bhagavato,",
  "tha-ap342:11.4_0": "dhammaṁ sacchikariṁ ahaṁ.",
  "tha-ap342:12.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap342:12.2_0": "bhavā sabbe samūhatā;",
  "tha-ap342:12.3_0": "Nāgova bandhanaṁ chetvā,",
  "tha-ap342:12.4_0": "viharāmi anāsavo.",
  "tha-ap342:13.1_0": "Satasahassito kappe,",
  "tha-ap342:13.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap342:13.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap342:13.4_0": "ekapadumassidaṁ phalaṁ.",
  "tha-ap342:14.1_0": "Paṭisambhidā catasso,",
  "tha-ap342:14.2_0": "…pe…",
  "tha-ap342:14.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap342:15.1_0": "Itthaṁ sudaṁ āyasmā ukkhittapadumiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap342:16.1_0": "Ukkhittapadumiyattherassāpadānaṁ dasamaṁ.",
  "tha-ap342:17.1_0": "Gandhodakavaggo catutiṁsatimo.",
  "tha-ap342:18.1_0": "Tassuddānaṁ",
  "tha-ap342:19.1_0": "Gandhadhūpo udakañca,",
  "tha-ap342:19.2_0": "punnāga ekadussakā;",
  "tha-ap342:19.3_0": "Phusito ca pabhaṅkaro,",
  "tha-ap342:19.4_0": "kuṭido uttarīyako.",
  "tha-ap342:20.1_0": "Savanī ekapadumī,",
  "tha-ap342:20.2_0": "gāthāyo sabbapiṇḍitā;",
  "tha-ap342:20.3_0": "Ekaṁ gāthāsatañceva,",
  "tha-ap342:20.4_0": "catutālīsameva ca."
}