{
  "ja326:0.1_0": "Jātaka",
  "ja326:0.2_0": "Catukkanipāta",
  "ja326:0.3_0": "Kuṭidūsakavagga",
  "ja326:0.4_0": "6. Kakkārujātaka",
  "ja326:1.1_0": "“Kāyena yo nāvahare,",
  "ja326:1.2_0": "vācāya na musā bhaṇe;",
  "ja326:1.3_0": "Yaso laddhā na majjeyya,",
  "ja326:1.4_0": "sa ve kakkārumarahati”.",
  "ja326:2.1_0": "“Dhammena vittameseyya,",
  "ja326:2.2_0": "na nikatyā dhanaṁ hare;",
  "ja326:2.3_0": "Bhoge laddhā na majjeyya,",
  "ja326:2.4_0": "sa ve kakkārumarahati”.",
  "ja326:3.1_0": "“Yassa cittaṁ ahāliddaṁ,",
  "ja326:3.2_0": "saddhā ca avirāginī;",
  "ja326:3.3_0": "Eko sāduṁ na bhuñjeyya,",
  "ja326:3.4_0": "sa ve kakkārumarahati”.",
  "ja326:4.1_0": "“Sammukhā vā tirokkhā vā,",
  "ja326:4.2_0": "Yo sante na paribhāsati;",
  "ja326:4.3_0": "Yathāvādī tathākārī,",
  "ja326:4.4_0": "Sa ve kakkārumarahatī”ti.",
  "ja326:5.1_0": "Kakkārujātakaṁ chaṭṭhaṁ."
}