{
  "thag1.81:0.1_0": "Theragāthā 1.81",
  "thag1.81:0.2_0": "Ekakanipāta",
  "thag1.81:0.3_0": "Navamavagga",
  "thag1.81:0.4_0": "Samitiguttattheragāthā",
  "thag1.81:1.1_0": "“Yaṁ mayā pakataṁ pāpaṁ,",
  "thag1.81:1.2_0": "pubbe aññāsu jātisu;",
  "thag1.81:1.3_0": "Idheva taṁ vedanīyaṁ,",
  "thag1.81:1.4_0": "vatthu aññaṁ na vijjatī”ti.",
  "thag1.81:1.5_0": "… Samitigutto thero …."
}