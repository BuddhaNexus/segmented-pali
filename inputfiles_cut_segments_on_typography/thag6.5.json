{
  "thag6.5:0.1_0": "Theragāthā 6.5",
  "thag6.5:0.2_0": "Chakkanipāta",
  "thag6.5:0.3_0": "Paṭhamavagga",
  "thag6.5:0.4_0": "Mālukyaputtattheragāthā",
  "thag6.5:1.1_0": "“Manujassa pamattacārino,",
  "thag6.5:1.2_0": "Taṇhā vaḍḍhati māluvā viya;",
  "thag6.5:1.3_0": "So plavatī hurā huraṁ,",
  "thag6.5:1.4_0": "Phalamicchaṁva vanasmi vānaro.",
  "thag6.5:2.1_0": "Yaṁ esā sahate jammī,",
  "thag6.5:2.2_0": "taṇhā loke visattikā;",
  "thag6.5:2.3_0": "Sokā tassa pavaḍḍhanti,",
  "thag6.5:2.4_0": "abhivaṭṭhaṁva bīraṇaṁ.",
  "thag6.5:3.1_0": "Yo cetaṁ sahate jammiṁ,",
  "thag6.5:3.2_0": "taṇhaṁ loke duraccayaṁ;",
  "thag6.5:3.3_0": "Sokā tamhā papatanti,",
  "thag6.5:3.4_0": "udabindūva pokkharā.",
  "thag6.5:4.1_0": "Taṁ vo vadāmi bhaddaṁ vo,",
  "thag6.5:4.2_0": "yāvantettha samāgatā;",
  "thag6.5:4.3_0": "Taṇhāya mūlaṁ khaṇatha,",
  "thag6.5:4.4_0": "usīratthova bīraṇaṁ;",
  "thag6.5:4.5_0": "Mā vo naḷaṁva sotova,",
  "thag6.5:4.6_0": "māro bhañji punappunaṁ.",
  "thag6.5:5.1_0": "Karotha buddhavacanaṁ,",
  "thag6.5:5.2_0": "khaṇo vo mā upaccagā;",
  "thag6.5:5.3_0": "Khaṇātītā hi socanti,",
  "thag6.5:5.4_0": "nirayamhi samappitā.",
  "thag6.5:6.1_0": "Pamādo rajo pamādo,",
  "thag6.5:6.2_0": "pamādānupatito rajo;",
  "thag6.5:6.3_0": "Appamādena vijjāya,",
  "thag6.5:6.4_0": "abbahe sallamattano”ti.",
  "thag6.5:6.5_0": "… Mālukyaputto thero …."
}