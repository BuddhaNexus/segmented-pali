{
  "tika-s0305t7:0_0": "8. Anuruddhasaṁyuttaṁ",
  "tika-s0305t7:1_0": "1. Rahogatavaggo",
  "tika-s0305t7:2_0": "1-2. Paṭhamarahogatasuttādivaṇṇanā",
  "tika-s0305t7:3_0": "899-",
  "tika-s0305t7:3_1": "900.Chattiṁsāyaṭhānesūti ajjhattaṁ kāye samudayadhammānupassī, vayo,",
  "tika-s0305t7:3_2": "samudayavayo,",
  "tika-s0305t7:3_3": "bahiddhā samudayo, vayo,",
  "tika-s0305t7:3_4": "samudayavayo,",
  "tika-s0305t7:3_5": "ajjhattabahiddhā samudayo, vayo,",
  "tika-s0305t7:3_6": "samudayavayadhammānupassīti nava anupassanā,",
  "tika-s0305t7:3_7": "tathā vedanāya citte dhammesūti evaṁ chattiṁsāya ṭhānesu.",
  "tika-s0305t7:3_8": "Dutiye dvādasasu ṭhānesūti kāye ajjhattaṁ bahiddhā ajjhattabahiddhā,",
  "tika-s0305t7:3_9": "vedanāya citte dhammesūti evaṁ dvādasasu ṭhānesu.",
  "tika-s0305t7:4_0": "3. Sutanusuttavaṇṇanā",
  "tika-s0305t7:5_0": "901.Imāya pāḷiyāti imāya hīnattikapāḷiyā.",
  "tika-s0305t7:5_1": "Ime dhammā hīnā lāmakaṭṭhena.",
  "tika-s0305t7:5_2": "Ime dhammā majjhimā hīnapaṇītānaṁ majjhe bhavāti.",
  "tika-s0305t7:5_3": "Uttamaṭṭhena attappakaṭṭhena padhānabhāvaṁ nītāti paṇītā.",
  "tika-s0305t7:6_0": "4-7. Paṭhamakaṇḍakīsuttādivaṇṇanā",
  "tika-s0305t7:7_0": "902- 905.",
  "tika-s0305t7:7_1": "Kaṇḍakā etissā atthīti kaṇḍakī,",
  "tika-s0305t7:7_2": "karamandagaccho.",
  "tika-s0305t7:7_3": "Osadhibhāvāpekkhāya itthiliṅganiddeso,",
  "tika-s0305t7:7_4": "tabbahulatāya taṁ vanaṁ “kaṇḍakīvana”nteva paññāyittha.",
  "tika-s0305t7:7_5": "Sahassalokanti sahassacakkavāḷalokaṁ.",
  "tika-s0305t7:7_6": "Dasacakkavāḷasahassaṁ ekāvajjanassa āpāthaṁ āgacchati tathā ālokavaḍḍhanassa katattā.",
  "tika-s0305t7:8_0": "9. Ambapālivanasuttavaṇṇanā",
  "tika-s0305t7:9_0": "907.Arahattabhāvadīpakanti arahattassa atthibhāvadīpakaṁ. “Arahattabhāvadīpika”nti vā pāṭho.",
  "tika-s0305t7:10_0": "Rahogatavaggavaṇṇanā niṭṭhitā.",
  "tika-s0305t7:11_0": "2. Dutiyavaggavaṇṇanā",
  "tika-s0305t7:12_0": "909-",
  "tika-s0305t7:12_1": "922.Dasabalañāṇanti dasavidhabalañāṇaṁ.",
  "tika-s0305t7:12_2": "Ekadesenāti padesavasena.",
  "tika-s0305t7:12_3": "Sāvakānampi attano abhinīhārānurūpaṁ ñāṇaṁ pavattatīti te kālapadesavasena ceva yathāparicayasattapadesavasena ca ṭhānānīti jānanti,",
  "tika-s0305t7:12_4": "sammāsambuddhānaṁ pana anantañāṇatāya sabbattheva appaṭihatameva ñāṇanti āha –",
  "tika-s0305t7:12_5": "“sabbaññubuddhānaṁ panā”tiādi.",
  "tika-s0305t7:12_6": "Etaṁ dasabalañāṇaṁ anantavisayattā nippadesaṁ anūnatāya sabbākāraparipūraṁ.",
  "tika-s0305t7:13_0": "Dutiyavaggavaṇṇanā niṭṭhitā.",
  "tika-s0305t7:14_0": "Anuruddhasaṁyuttavaṇṇanā niṭṭhitā."
}