{
  "kv21.7:0.1_0": "Kathāvatthu",
  "kv21.7:0.2_0": "Mahāpaṇṇāsaka",
  "kv21.7:0.3_0": "Ekavīsatimavagga",
  "kv21.7:0.4_0": "Dhammakathā",
  "kv21.7:1.1_0": "Sabbe dhammā niyatāti?",
  "kv21.7:1.2_0": "Āmantā.",
  "kv21.7:1.3_0": "Micchattaniyatāti?",
  "kv21.7:1.4_0": "Na hevaṁ vattabbe …pe…",
  "kv21.7:1.5_0": "sammattaniyatāti?",
  "kv21.7:1.6_0": "Na hevaṁ vattabbe …pe…",
  "kv21.7:1.7_0": "natthi aniyato rāsīti?",
  "kv21.7:1.8_0": "Na hevaṁ vattabbe …pe…",
  "kv21.7:1.9_0": "nanu atthi aniyato rāsīti?",
  "kv21.7:1.10_0": "Āmantā.",
  "kv21.7:1.11_0": "Hañci atthi aniyato rāsi, no ca vata re vattabbe—",
  "kv21.7:1.12_0": "“sabbe dhammā niyatā”ti.",
  "kv21.7:2.1_0": "Sabbe dhammā niyatāti?",
  "kv21.7:2.2_0": "Āmantā.",
  "kv21.7:2.3_0": "Nanu tayo rāsī vuttā bhagavatā—",
  "kv21.7:2.4_0": "“micchattaniyato rāsi, sammattaniyato rāsi, aniyato rāsī”ti?",
  "kv21.7:2.5_0": "Āmantā.",
  "kv21.7:2.6_0": "Hañci tayo rāsī vuttā bhagavatā—",
  "kv21.7:2.7_0": "“micchattaniyato rāsi, sammattaniyato rāsi, aniyato rāsi”, no ca vata re vattabbe—",
  "kv21.7:2.8_0": "“sabbe dhammā niyatā”ti.",
  "kv21.7:3.1_0": "Rūpaṁ rūpaṭṭhena niyatanti?",
  "kv21.7:3.2_0": "Āmantā.",
  "kv21.7:3.3_0": "Micchattaniyatanti?",
  "kv21.7:3.4_0": "Na hevaṁ vattabbe …pe…",
  "kv21.7:3.5_0": "sammattaniyatanti?",
  "kv21.7:3.6_0": "Na hevaṁ vattabbe …pe…",
  "kv21.7:3.7_0": "vedanā …pe…",
  "kv21.7:3.8_0": "saññā …pe…",
  "kv21.7:3.9_0": "saṅkhārā …pe…",
  "kv21.7:3.10_0": "viññāṇaṁ viññāṇaṭṭhena niyatanti?",
  "kv21.7:3.11_0": "Āmantā.",
  "kv21.7:3.12_0": "Micchattaniyatanti?",
  "kv21.7:3.13_0": "Na hevaṁ vattabbe …pe…",
  "kv21.7:3.14_0": "sammattaniyatanti?",
  "kv21.7:3.15_0": "Na hevaṁ vattabbe …pe….",
  "kv21.7:4.1_0": "Na vattabbaṁ—",
  "kv21.7:4.2_0": "rūpaṁ rūpaṭṭhena niyataṁ …pe…",
  "kv21.7:4.3_0": "vedanā …pe…",
  "kv21.7:4.4_0": "saññā …pe…",
  "kv21.7:4.5_0": "saṅkhārā …pe…",
  "kv21.7:4.6_0": "viññāṇaṁ viññāṇaṭṭhena niyatanti?",
  "kv21.7:4.7_0": "Āmantā.",
  "kv21.7:4.8_0": "Rūpaṁ vedanā hoti …pe…",
  "kv21.7:4.9_0": "saññā hoti …",
  "kv21.7:4.10_0": "saṅkhārā honti …",
  "kv21.7:4.11_0": "viññāṇaṁ hoti …",
  "kv21.7:4.12_0": "vedanā …pe…",
  "kv21.7:4.13_0": "saññā …pe…",
  "kv21.7:4.14_0": "saṅkhārā …pe…",
  "kv21.7:4.15_0": "viññāṇaṁ rūpaṁ hoti …pe…",
  "kv21.7:4.16_0": "vedanā hoti …",
  "kv21.7:4.17_0": "saññā hoti …",
  "kv21.7:4.18_0": "saṅkhārā hontīti?",
  "kv21.7:4.19_0": "Na hevaṁ vattabbe.",
  "kv21.7:4.20_0": "Tena hi rūpaṁ rūpaṭṭhena niyataṁ, vedanā …pe…",
  "kv21.7:4.21_0": "saññā …pe…",
  "kv21.7:4.22_0": "saṅkhārā …pe…",
  "kv21.7:4.23_0": "viññāṇaṁ viññāṇaṭṭhena niyatanti.",
  "kv21.7:4.24_0": "Dhammakathā niṭṭhitā."
}