{
  "kv15.5:0.1_0": "Kathāvatthu",
  "kv15.5:0.2_0": "Mahāpaṇṇāsaka",
  "kv15.5:0.3_0": "Pannarasamavagga",
  "kv15.5:0.4_0": "Āsavakathā",
  "kv15.5:1.1_0": "Cattāro āsavā anāsavāti?",
  "kv15.5:1.2_0": "Āmantā.",
  "kv15.5:1.3_0": "Maggo phalaṁ nibbānaṁ, sotāpattimaggo sotāpattiphalaṁ …pe… bojjhaṅgoti?",
  "kv15.5:1.4_0": "Na hevaṁ vattabbe …pe….",
  "kv15.5:2.1_0": "Na vattabbaṁ—",
  "kv15.5:2.2_0": "“cattāro āsavā anāsavā”ti?",
  "kv15.5:2.3_0": "Āmantā.",
  "kv15.5:2.4_0": "Atthaññeva āsavā yehi āsavehi te āsavā sāsavā hontīti?",
  "kv15.5:2.5_0": "Na hevaṁ vattabbe.",
  "kv15.5:2.6_0": "Tena hi cattāro āsavā anāsavāti.",
  "kv15.5:2.7_0": "Āsavakathā niṭṭhitā."
}