{
  "kv17.6:0.1_0": "Kathāvatthu",
  "kv17.6:0.2_0": "Mahāpaṇṇāsaka",
  "kv17.6:0.3_0": "Sattarasamavagga",
  "kv17.6:0.4_0": "Navattabbaṁsaṅghodakkhiṇaṁpaṭiggaṇhātikathā",
  "kv17.6:1.1_0": "Na vattabbaṁ—",
  "kv17.6:1.2_0": "“saṅgho dakkhiṇaṁ paṭiggaṇhātī”ti?",
  "kv17.6:1.3_0": "Āmantā.",
  "kv17.6:1.4_0": "Nanu saṅgho āhuneyyo pāhuneyyo dakkhiṇeyyo añjalikaraṇīyo anuttaraṁ puññakkhettaṁ lokassāti?",
  "kv17.6:1.5_0": "Āmantā.",
  "kv17.6:1.6_0": "Hañci saṅgho āhuneyyo pāhuneyyo dakkhiṇeyyo añjalikaraṇīyo anuttaraṁ puññakkhettaṁ lokassa,",
  "kv17.6:1.6_1": "tena vata re vattabbe—",
  "kv17.6:1.7_0": "“saṅgho dakkhiṇaṁ paṭiggaṇhātī”ti.",
  "kv17.6:2.1_0": "Na vattabbaṁ—",
  "kv17.6:2.2_0": "“saṅgho dakkhiṇaṁ paṭiggaṇhātī”ti?",
  "kv17.6:2.3_0": "Āmantā.",
  "kv17.6:2.4_0": "Nanu cattāro purisayugā aṭṭha purisapuggalā dakkhiṇeyyā vuttā bhagavatāti?",
  "kv17.6:2.5_0": "Āmantā.",
  "kv17.6:2.6_0": "Hañci cattāro purisayugā aṭṭha purisapuggalā dakkhiṇeyyā vuttā bhagavatā, tena vata re vattabbe—",
  "kv17.6:2.7_0": "“saṅgho dakkhiṇaṁ paṭiggaṇhātī”ti.",
  "kv17.6:3.1_0": "Na vattabbaṁ—",
  "kv17.6:3.2_0": "“saṅgho dakkhiṇaṁ paṭiggaṇhātī”ti?",
  "kv17.6:3.3_0": "Āmantā.",
  "kv17.6:3.4_0": "Nanu atthi keci saṅghassa dānaṁ dentīti?",
  "kv17.6:3.5_0": "Āmantā.",
  "kv17.6:3.6_0": "Hañci atthi keci saṅghassa dānaṁ denti, tena vata re vattabbe—",
  "kv17.6:3.7_0": "“saṅgho dakkhiṇaṁ paṭiggaṇhātī”ti.",
  "kv17.6:3.8_0": "Nanu atthi keci saṅghassa cīvaraṁ denti …pe…",
  "kv17.6:3.9_0": "piṇḍapātaṁ denti …",
  "kv17.6:3.10_0": "senāsanaṁ denti …",
  "kv17.6:3.11_0": "gilānapaccayabhesajjaparikkhāraṁ denti …",
  "kv17.6:3.12_0": "khādanīyaṁ denti …",
  "kv17.6:3.13_0": "bhojanīyaṁ denti …pe…",
  "kv17.6:3.14_0": "pānīyaṁ dentīti?",
  "kv17.6:3.15_0": "Āmantā.",
  "kv17.6:3.16_0": "Hañci atthi keci saṅghassa pānīyaṁ denti, tena vata re vattabbe—",
  "kv17.6:3.17_0": "“saṅgho dakkhiṇaṁ paṭiggaṇhātī”ti.",
  "kv17.6:4.1_0": "Na vattabbaṁ—",
  "kv17.6:4.2_0": "“saṅgho dakkhiṇaṁ paṭiggaṇhātī”ti?",
  "kv17.6:4.3_0": "Āmantā.",
  "kv17.6:4.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv17.6:5.1_0": "“Āhutiṁ jātavedova,",
  "kv17.6:5.2_0": "mahāmeghaṁva medanī;",
  "kv17.6:5.3_0": "Saṅgho samādhisampanno,",
  "kv17.6:5.4_0": "paṭiggaṇhāti dakkhiṇan”ti.",
  "kv17.6:6.1_0": "Attheva suttantoti?",
  "kv17.6:6.2_0": "Āmantā.",
  "kv17.6:6.3_0": "Tena hi saṅgho dakkhiṇaṁ paṭiggaṇhātīti.",
  "kv17.6:7.1_0": "Saṅgho dakkhiṇaṁ paṭiggaṇhātīti?",
  "kv17.6:7.2_0": "Āmantā.",
  "kv17.6:7.3_0": "Maggo paṭiggaṇhāti, phalaṁ paṭiggaṇhātīti?",
  "kv17.6:7.4_0": "Na hevaṁ vattabbe …pe….",
  "kv17.6:7.5_0": "Na vattabbaṁ saṅgho dakkhiṇaṁ paṭiggaṇhātītikathā niṭṭhitā."
}