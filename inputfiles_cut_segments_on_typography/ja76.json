{
  "ja76:0.1_0": "Jātaka",
  "ja76:0.2_0": "Ekakanipāta",
  "ja76:0.3_0": "Varuṇavagga",
  "ja76:0.4_0": "6. Asaṅkiyajātaka",
  "ja76:1.1_0": "“Asaṅkiyomhi gāmamhi,",
  "ja76:1.2_0": "araññe natthi me bhayaṁ;",
  "ja76:1.3_0": "Ujuṁ maggaṁ samārūḷho,",
  "ja76:1.4_0": "mettāya karuṇāya cā”ti.",
  "ja76:2.1_0": "Asaṅkiyajātakaṁ chaṭṭhaṁ."
}