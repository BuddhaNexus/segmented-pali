{
  "pp1.7:0.1_0": "Puggalapaññatti",
  "pp1.7:0.2_0": "Mātikā",
  "pp1.7:0.3_0": "7. Sattakauddesa",
  "pp1.7:1.1_0": "Satta puggalā—",
  "pp1.7:2.1_0": "Satta udakūpamā puggalā.",
  "pp1.7:2.2_0": "Sakiṁ nimuggo nimuggova hoti,",
  "pp1.7:2.2_1": "ummujjitvā nimujjati,",
  "pp1.7:2.2_2": "ummujjitvā ṭhito hoti,",
  "pp1.7:2.2_3": "ummujjitvā vipassati viloketi,",
  "pp1.7:2.2_4": "ummujjitvā patarati,",
  "pp1.7:2.2_5": "ummujjitvā patigādhappatto hoti,",
  "pp1.7:2.2_6": "ummujjitvā tiṇṇo hoti pāraṅgato thale tiṭṭhati brāhmaṇo.",
  "pp1.7:3.1_0": "Ubhatobhāgavimutto,",
  "pp1.7:3.1_1": "paññāvimutto,",
  "pp1.7:3.1_2": "kāyasakkhī,",
  "pp1.7:3.1_3": "diṭṭhippatto,",
  "pp1.7:3.1_4": "saddhāvimutto,",
  "pp1.7:3.1_5": "dhammānusārī,",
  "pp1.7:3.1_6": "saddhānusārī.",
  "pp1.7:4.1_0": "Sattakaṁ."
}