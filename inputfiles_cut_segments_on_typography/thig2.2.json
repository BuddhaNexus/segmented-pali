{
  "thig2.2:0.1_0": "Therīgāthā 2.2",
  "thig2.2:0.2_0": "Dukanipāta",
  "thig2.2:0.3_0": "Jentātherīgāthā",
  "thig2.2:1.1_0": "“Ye ime satta bojjhaṅgā,",
  "thig2.2:1.2_0": "maggā nibbānapattiyā;",
  "thig2.2:1.3_0": "Bhāvitā te mayā sabbe,",
  "thig2.2:1.4_0": "yathā buddhena desitā.",
  "thig2.2:2.1_0": "Diṭṭho hi me so bhagavā,",
  "thig2.2:2.2_0": "antimoyaṁ samussayo;",
  "thig2.2:2.3_0": "Vikkhīṇo jātisaṁsāro,",
  "thig2.2:2.4_0": "natthi dāni punabbhavo”ti.",
  "thig2.2:3.1_0": "Itthaṁ sudaṁ jentā therī gāthāyo abhāsitthāti."
}