{
  "tha-ap75:0.1_0": "Therāpadāna",
  "tha-ap75:0.2_0": "Nāgasamālavagga",
  "tha-ap75:0.3_0": "3 Buddhasaññakattheraapadāna",
  "tha-ap75:1.1_0": "“Dumagge paṁsukūlikaṁ,",
  "tha-ap75:1.2_0": "laggaṁ disvāna satthuno;",
  "tha-ap75:1.3_0": "Tato tamañjaliṁ katvā,",
  "tha-ap75:1.4_0": "paṁsukūlaṁ avandahaṁ.",
  "tha-ap75:2.1_0": "Dvenavute ito kappe,",
  "tha-ap75:2.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap75:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap75:2.4_0": "buddhasaññāyidaṁ phalaṁ.",
  "tha-ap75:3.1_0": "Ito catutthake kappe,",
  "tha-ap75:3.2_0": "dumasārosi khattiyo;",
  "tha-ap75:3.3_0": "Cāturanto vijitāvī,",
  "tha-ap75:3.4_0": "cakkavattī mahabbalo.",
  "tha-ap75:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap75:4.2_0": "…pe…",
  "tha-ap75:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap75:5.1_0": "Itthaṁ sudaṁ āyasmā buddhasaññako thero imā gāthāyo abhāsitthāti.",
  "tha-ap75:6.1_0": "Buddhasaññakattherassāpadānaṁ tatiyaṁ."
}