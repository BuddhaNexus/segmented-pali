{
  "ja538:0.1_0": "Jātaka",
  "ja538:0.2_0": "Mahānipāta",
  "ja538:0.3_0": "Mūgapakkhavagga",
  "ja538:0.4_0": "1 Mūgapakkhajātaka",
  "ja538:1.1_0": "“Mā paṇḍiccayaṁ vibhāvaya,",
  "ja538:1.2_0": "Bālamato bhava sabbapāṇinaṁ;",
  "ja538:1.3_0": "Sabbo taṁ jano ocināyatu,",
  "ja538:1.4_0": "Evaṁ tava attho bhavissati”.",
  "ja538:2.1_0": "“Karomi te taṁ vacanaṁ,",
  "ja538:2.2_0": "yaṁ maṁ bhaṇasi devate;",
  "ja538:2.3_0": "Atthakāmāsi me amma,",
  "ja538:2.4_0": "hitakāmāsi devate”.",
  "ja538:3.1_0": "“Kiṁ nu santaramānova,",
  "ja538:3.2_0": "kāsuṁ khaṇasi sārathi;",
  "ja538:3.3_0": "Puṭṭho me samma akkhāhi,",
  "ja538:3.4_0": "kiṁ kāsuyā karissasi”.",
  "ja538:4.1_0": "“Rañño mūgo ca pakkho ca,",
  "ja538:4.2_0": "putto jāto acetaso;",
  "ja538:4.3_0": "Somhi raññā samajjhiṭṭho,",
  "ja538:4.4_0": "puttaṁ me nikhaṇaṁ vane”.",
  "ja538:5.1_0": "“Na badhiro na mūgosmi,",
  "ja538:5.2_0": "na pakkho na ca vīkalo;",
  "ja538:5.3_0": "Adhammaṁ sārathi kayirā,",
  "ja538:5.4_0": "mañce tvaṁ nikhaṇaṁ vane.",
  "ja538:6.1_0": "Ūrū bāhuñca me passa,",
  "ja538:6.2_0": "bhāsitañca suṇohi me;",
  "ja538:6.3_0": "Adhammaṁ sārathi kayirā,",
  "ja538:6.4_0": "mañce tvaṁ nikhaṇaṁ vane”.",
  "ja538:7.1_0": "“Devatā nusi gandhabbo,",
  "ja538:7.2_0": "adu sakko purindado;",
  "ja538:7.3_0": "Ko vā tvaṁ kassa vā putto,",
  "ja538:7.4_0": "kathaṁ jānemu taṁ mayaṁ”.",
  "ja538:8.1_0": "“Namhi devo na gandhabbo,",
  "ja538:8.2_0": "nāpi sakko purindado;",
  "ja538:8.3_0": "Kāsirañño ahaṁ putto,",
  "ja538:8.4_0": "yaṁ kāsuyā nikhaññasi.",
  "ja538:9.1_0": "Tassa rañño ahaṁ putto,",
  "ja538:9.2_0": "yaṁ tvaṁ sammūpajīvasi;",
  "ja538:9.3_0": "Adhammaṁ sārathi kayirā,",
  "ja538:9.4_0": "mañce tvaṁ nikhaṇaṁ vane.",
  "ja538:10.1_0": "Yassa rukkhassa chāyāya,",
  "ja538:10.2_0": "nisīdeyya sayeyya vā;",
  "ja538:10.3_0": "Na tassa sākhaṁ bhañjeyya,",
  "ja538:10.4_0": "mittadubbho hi pāpako.",
  "ja538:11.1_0": "Yathā rukkho tathā rājā,",
  "ja538:11.2_0": "yathā sākhā tathā ahaṁ;",
  "ja538:11.3_0": "Yathā chāyūpago poso,",
  "ja538:11.4_0": "evaṁ tvamasi sārathi;",
  "ja538:11.5_0": "Adhammaṁ sārathi kayirā,",
  "ja538:11.6_0": "mañce tvaṁ nikhaṇaṁ vane.",
  "ja538:12.1_0": "Pahūtabhakkho bhavati,",
  "ja538:12.2_0": "vippavuttho sakaṁ gharā;",
  "ja538:12.3_0": "Bahū naṁ upajīvanti,",
  "ja538:12.4_0": "yo mittānaṁ na dubbhati.",
  "ja538:13.1_0": "Yaṁ yaṁ janapadaṁ yāti,",
  "ja538:13.2_0": "nigame rājadhāniyo;",
  "ja538:13.3_0": "Sabbattha pūjito hoti,",
  "ja538:13.4_0": "yo mittānaṁ na dubbhati.",
  "ja538:14.1_0": "Nāssa corā pasāhanti,",
  "ja538:14.2_0": "nātimaññanti khattiyā;",
  "ja538:14.3_0": "Sabbe amitte tarati,",
  "ja538:14.4_0": "yo mittānaṁ na dubbhati.",
  "ja538:15.1_0": "Akkuddho sagharaṁ eti,",
  "ja538:15.2_0": "sabhāyaṁ paṭinandito;",
  "ja538:15.3_0": "Ñātīnaṁ uttamo hoti,",
  "ja538:15.4_0": "yo mittānaṁ na dubbhati.",
  "ja538:16.1_0": "Sakkatvā sakkato hoti,",
  "ja538:16.2_0": "garu hoti sagāravo;",
  "ja538:16.3_0": "Vaṇṇakittibhato hoti,",
  "ja538:16.4_0": "yo mittānaṁ na dubbhati.",
  "ja538:17.1_0": "Pūjako labhate pūjaṁ,",
  "ja538:17.2_0": "vandako paṭivandanaṁ;",
  "ja538:17.3_0": "Yaso kittiñca pappoti,",
  "ja538:17.4_0": "yo mittānaṁ na dubbhati.",
  "ja538:18.1_0": "Aggi yathā pajjalati,",
  "ja538:18.2_0": "devatāva virocati;",
  "ja538:18.3_0": "Siriyā ajahito hoti,",
  "ja538:18.4_0": "yo mittānaṁ na dubbhati.",
  "ja538:19.1_0": "Gāvo tassa pajāyanti,",
  "ja538:19.2_0": "khette vuttaṁ virūhati;",
  "ja538:19.3_0": "Vuttānaṁ phalamasnāti,",
  "ja538:19.4_0": "yo mittānaṁ na dubbhati.",
  "ja538:20.1_0": "Darito pabbatāto vā,",
  "ja538:20.2_0": "rukkhato patito naro;",
  "ja538:20.3_0": "Cuto patiṭṭhaṁ labhati,",
  "ja538:20.4_0": "yo mittānaṁ na dubbhati.",
  "ja538:21.1_0": "Virūḷhamūlasantānaṁ,",
  "ja538:21.2_0": "nigrodhamiva māluto;",
  "ja538:21.3_0": "Amittā nappasāhanti,",
  "ja538:21.4_0": "yo mittānaṁ na dubbhati”.",
  "ja538:22.1_0": "“Ehi taṁ paṭinessāmi,",
  "ja538:22.2_0": "rājaputta sakaṁ gharaṁ;",
  "ja538:22.3_0": "Rajjaṁ kārehi bhaddante,",
  "ja538:22.4_0": "kiṁ araññe karissasi”.",
  "ja538:23.1_0": "“Alaṁ me tena rajjena,",
  "ja538:23.2_0": "ñātakehi dhanena vā;",
  "ja538:23.3_0": "Yaṁ me adhammacariyāya,",
  "ja538:23.4_0": "rajjaṁ labbhetha sārathi”.",
  "ja538:24.1_0": "“Puṇṇapattaṁ maṁ lābhehi,",
  "ja538:24.2_0": "rājaputta ito gato;",
  "ja538:24.3_0": "Pitā mātā ca me dajjuṁ,",
  "ja538:24.4_0": "rājaputta tayī gate.",
  "ja538:25.1_0": "Orodhā ca kumārā ca,",
  "ja538:25.2_0": "vesiyānā ca brāhmaṇā;",
  "ja538:25.3_0": "Tepi attamanā dajjuṁ,",
  "ja538:25.4_0": "rājaputta tayī gate.",
  "ja538:26.1_0": "Hatthārohā anīkaṭṭhā,",
  "ja538:26.2_0": "rathikā pattikārakā;",
  "ja538:26.3_0": "Tepi attamanā dajjuṁ,",
  "ja538:26.4_0": "rājaputta tayī gate.",
  "ja538:27.1_0": "Bahudhaññā jānapadā,",
  "ja538:27.2_0": "negamā ca samāgatā;",
  "ja538:27.3_0": "Upāyanāni me dajjuṁ,",
  "ja538:27.4_0": "rājaputta tayī gate”.",
  "ja538:28.1_0": "“Pitu mātu cahaṁ catto,",
  "ja538:28.2_0": "raṭṭhassa nigamassa ca;",
  "ja538:28.3_0": "Atho sabbakumārānaṁ,",
  "ja538:28.4_0": "natthi mayhaṁ sakaṁ gharaṁ.",
  "ja538:29.1_0": "Anuññāto ahaṁ matyā,",
  "ja538:29.2_0": "sañcatto pitarā mahaṁ;",
  "ja538:29.3_0": "Ekoraññe pabbajito,",
  "ja538:29.4_0": "na kāme abhipatthaye.",
  "ja538:30.1_0": "Api ataramānānaṁ,",
  "ja538:30.2_0": "phalāsāva samijjhati;",
  "ja538:30.3_0": "Vipakkabrahmacariyosmi,",
  "ja538:30.4_0": "evaṁ jānāhi sārathi.",
  "ja538:31.1_0": "Api ataramānānaṁ,",
  "ja538:31.2_0": "sammadattho vipaccati;",
  "ja538:31.3_0": "Vipakkabrahmacariyosmi,",
  "ja538:31.4_0": "nikkhanto akutobhayo”.",
  "ja538:32.1_0": "“Evaṁ vaggukatho santo,",
  "ja538:32.2_0": "visaṭṭhavacano casi;",
  "ja538:32.3_0": "Kasmā pitu ca mātucca,",
  "ja538:32.4_0": "santike na bhaṇī tadā”.",
  "ja538:33.1_0": "“Nāhaṁ asandhitā pakkho,",
  "ja538:33.2_0": "na badhiro asotatā;",
  "ja538:33.3_0": "Nāhaṁ ajivhatā mūgo,",
  "ja538:33.4_0": "mā maṁ mūgamadhārayi.",
  "ja538:34.1_0": "Purimaṁ sarāmahaṁ jātiṁ,",
  "ja538:34.2_0": "yattha rajjamakārayiṁ;",
  "ja538:34.3_0": "Kārayitvā tahiṁ rajjaṁ,",
  "ja538:34.4_0": "pāpatthaṁ nirayaṁ bhusaṁ.",
  "ja538:35.1_0": "Vīsatiñceva vassāni,",
  "ja538:35.2_0": "tahiṁ rajjamakārayiṁ;",
  "ja538:35.3_0": "Asītivassasahassāni,",
  "ja538:35.4_0": "nirayamhi apaccisaṁ.",
  "ja538:36.1_0": "Tassa rajjassahaṁ bhīto,",
  "ja538:36.2_0": "mā maṁ rajjābhisecayuṁ;",
  "ja538:36.3_0": "Tasmā pitu ca mātucca,",
  "ja538:36.4_0": "santike na bhaṇiṁ tadā.",
  "ja538:37.1_0": "Ucchaṅge maṁ nisādetvā,",
  "ja538:37.2_0": "pitā atthānusāsati;",
  "ja538:37.3_0": "Ekaṁ hanatha bandhatha,",
  "ja538:37.4_0": "ekaṁ khārāpatacchikaṁ;",
  "ja538:37.5_0": "Ekaṁ sūlasmiṁ uppetha,",
  "ja538:37.6_0": "iccassa manusāsati.",
  "ja538:38.1_0": "Tāyāhaṁ pharusaṁ sutvā,",
  "ja538:38.2_0": "vācāyo samudīritā;",
  "ja538:38.3_0": "Amūgo mūgavaṇṇena,",
  "ja538:38.4_0": "apakkho pakkhasammato;",
  "ja538:38.5_0": "Sake muttakarīsasmiṁ,",
  "ja538:38.6_0": "acchāhaṁ samparipluto.",
  "ja538:39.1_0": "Kasirañca parittañca,",
  "ja538:39.2_0": "tañca dukkhena saṁyutaṁ;",
  "ja538:39.3_0": "Komaṁ jīvitamāgamma,",
  "ja538:39.4_0": "veraṁ kayirātha kenaci.",
  "ja538:40.1_0": "Paññāya ca alābhena,",
  "ja538:40.2_0": "dhammassa ca adassanā;",
  "ja538:40.3_0": "Komaṁ jīvitamāgamma,",
  "ja538:40.4_0": "veraṁ kayirātha kenaci.",
  "ja538:41.1_0": "Api ataramānānaṁ,",
  "ja538:41.2_0": "phalāsāva samijjhati;",
  "ja538:41.3_0": "Vipakkabrahmacariyosmi,",
  "ja538:41.4_0": "evaṁ jānāhi sārathi.",
  "ja538:42.1_0": "Api ataramānānaṁ,",
  "ja538:42.2_0": "sammadattho vipaccati;",
  "ja538:42.3_0": "Vipakkabrahmacariyosmi,",
  "ja538:42.4_0": "nikkhanto akutobhayo”.",
  "ja538:43.1_0": "“Ahampi pabbajissāmi,",
  "ja538:43.2_0": "rājaputta tavantike;",
  "ja538:43.3_0": "Avhāyassu maṁ bhaddante,",
  "ja538:43.4_0": "pabbajjā mama ruccati”.",
  "ja538:44.1_0": "“Rathaṁ niyyādayitvāna,",
  "ja538:44.2_0": "anaṇo ehi sārathi;",
  "ja538:44.3_0": "Anaṇassa hi pabbajjā,",
  "ja538:44.4_0": "etaṁ isīhi vaṇṇitaṁ”.",
  "ja538:45.1_0": "“Yadeva tyāhaṁ vacanaṁ,",
  "ja538:45.2_0": "akaraṁ bhaddamatthu te;",
  "ja538:45.3_0": "Tadeva me tvaṁ vacanaṁ,",
  "ja538:45.4_0": "yācito kattumarahasi.",
  "ja538:46.1_0": "Idheva tāva acchassu,",
  "ja538:46.2_0": "yāva rājānamānaye;",
  "ja538:46.3_0": "Appeva te pitā disvā,",
  "ja538:46.4_0": "patīto sumano siyā”.",
  "ja538:47.1_0": "“Karomi tetaṁ vacanaṁ,",
  "ja538:47.2_0": "yaṁ maṁ bhaṇasi sārathi;",
  "ja538:47.3_0": "Ahampi daṭṭhukāmosmi,",
  "ja538:47.4_0": "pitaraṁ me idhāgataṁ.",
  "ja538:48.1_0": "Ehi samma nivattassu,",
  "ja538:48.2_0": "kusalaṁ vajjāsi ñātinaṁ;",
  "ja538:48.3_0": "Mātaraṁ pitaraṁ mayhaṁ,",
  "ja538:48.4_0": "vutto vajjāsi vandanaṁ”.",
  "ja538:49.1_0": "Tassa pāde gahetvāna,",
  "ja538:49.2_0": "katvā ca naṁ padakkhiṇaṁ;",
  "ja538:49.3_0": "Sārathi rathamāruyha,",
  "ja538:49.4_0": "rājadvāraṁ upāgami.",
  "ja538:50.1_0": "Suññaṁ mātā rathaṁ disvā,",
  "ja538:50.2_0": "ekaṁ sārathimāgataṁ;",
  "ja538:50.3_0": "Assupuṇṇehi nettehi,",
  "ja538:50.4_0": "rodantī naṁ udikkhati.",
  "ja538:51.1_0": "“Ayaṁ so sārathi eti,",
  "ja538:51.2_0": "nihantvā mama atrajaṁ;",
  "ja538:51.3_0": "Nihato nūna me putto,",
  "ja538:51.4_0": "pathabyā bhūmivaḍḍhano.",
  "ja538:52.1_0": "Amittā nūna nandanti,",
  "ja538:52.2_0": "patītā nūna verino;",
  "ja538:52.3_0": "Āgataṁ sārathiṁ disvā,",
  "ja538:52.4_0": "nihantvā mama atrajaṁ”.",
  "ja538:53.1_0": "Suññaṁ mātā rathaṁ disvā,",
  "ja538:53.2_0": "ekaṁ sārathimāgataṁ;",
  "ja538:53.3_0": "Assupuṇṇehi nettehi,",
  "ja538:53.4_0": "rodantī paripucchi naṁ.",
  "ja538:54.1_0": "“Kiṁ nu mūgo kiṁ nu pakkho,",
  "ja538:54.2_0": "kiṁ nu so vilapī tadā;",
  "ja538:54.3_0": "Nihaññamāno bhūmiyā,",
  "ja538:54.4_0": "taṁ me akkhāhi sārathi.",
  "ja538:55.1_0": "Kathaṁ hatthehi pādehi,",
  "ja538:55.2_0": "mūgapakkho vivajjayi;",
  "ja538:55.3_0": "Nihaññamāno bhūmiyā,",
  "ja538:55.4_0": "taṁ me akkhāhi pucchito”.",
  "ja538:56.1_0": "“Akkheyyaṁ te ahaṁ ayye,",
  "ja538:56.2_0": "dajjāsi abhayaṁ mama;",
  "ja538:56.3_0": "Yaṁ me sutaṁ vā diṭṭhaṁ vā,",
  "ja538:56.4_0": "rājaputtassa santike”.",
  "ja538:57.1_0": "“Abhayaṁ samma te dammi,",
  "ja538:57.2_0": "abhīto bhaṇa sārathi;",
  "ja538:57.3_0": "Yaṁ te sutaṁ vā diṭṭhaṁ vā,",
  "ja538:57.4_0": "rājaputtassa santike”.",
  "ja538:58.1_0": "“Na so mūgo na so pakkho,",
  "ja538:58.2_0": "visaṭṭhavacano ca so;",
  "ja538:58.3_0": "Rajjassa kira so bhīto,",
  "ja538:58.4_0": "akarā ālaye bahū.",
  "ja538:59.1_0": "Purimaṁ sarati so jātiṁ,",
  "ja538:59.2_0": "yattha rajjamakārayi;",
  "ja538:59.3_0": "Kārayitvā tahiṁ rajjaṁ,",
  "ja538:59.4_0": "pāpattha nirayaṁ bhusaṁ.",
  "ja538:60.1_0": "Vīsatiñceva vassāni,",
  "ja538:60.2_0": "tahiṁ rajjamakārayi;",
  "ja538:60.3_0": "Asītivassasahassāni,",
  "ja538:60.4_0": "nirayamhi apacci so.",
  "ja538:61.1_0": "Tassa rajjassa so bhīto,",
  "ja538:61.2_0": "mā maṁ rajjābhisecayuṁ;",
  "ja538:61.3_0": "Tasmā pitu ca mātucca,",
  "ja538:61.4_0": "santike na bhaṇī tadā.",
  "ja538:62.1_0": "Aṅgapaccaṅgasampanno,",
  "ja538:62.2_0": "ārohapariṇāhavā;",
  "ja538:62.3_0": "Visaṭṭhavacano pañño,",
  "ja538:62.4_0": "magge saggassa tiṭṭhati.",
  "ja538:63.1_0": "Sace tvaṁ daṭṭhukāmāsi,",
  "ja538:63.2_0": "rājaputtaṁ tavatrajaṁ;",
  "ja538:63.3_0": "Ehi taṁ pāpayissāmi,",
  "ja538:63.4_0": "yattha sammati temiyo”.",
  "ja538:64.1_0": "“Yojayantu rathe asse,",
  "ja538:64.2_0": "Kacchaṁ nāgāna bandhatha;",
  "ja538:64.3_0": "Udīrayantu saṅkhapaṇavā,",
  "ja538:64.4_0": "Vādantu ekapokkharā.",
  "ja538:65.1_0": "Vādantu bherī sannaddhā,",
  "ja538:65.2_0": "vaggū vādantu dundubhī;",
  "ja538:65.3_0": "Negamā ca maṁ anventu,",
  "ja538:65.4_0": "gacchaṁ puttanivedako.",
  "ja538:66.1_0": "Orodhā ca kumārā ca,",
  "ja538:66.2_0": "vesiyānā ca brāhmaṇā;",
  "ja538:66.3_0": "Khippaṁ yānāni yojentu,",
  "ja538:66.4_0": "gacchaṁ puttanivedako.",
  "ja538:67.1_0": "Hatthārohā anīkaṭṭhā,",
  "ja538:67.2_0": "rathikā pattikārakā;",
  "ja538:67.3_0": "Khippaṁ yānāni yojentu,",
  "ja538:67.4_0": "gacchaṁ puttanivedako.",
  "ja538:68.1_0": "Samāgatā jānapadā,",
  "ja538:68.2_0": "negamā ca samāgatā;",
  "ja538:68.3_0": "Khippaṁ yānāni yojentu,",
  "ja538:68.4_0": "gacchaṁ puttanivedako”.",
  "ja538:69.1_0": "Asse ca sārathī yutte,",
  "ja538:69.2_0": "sindhave sīghavāhane;",
  "ja538:69.3_0": "Rājadvāraṁ upāgacchuṁ,",
  "ja538:69.4_0": "“yuttā deva ime hayā”.",
  "ja538:70.1_0": "“Thūlā javena hāyanti,",
  "ja538:70.2_0": "kisā hāyanti thāmunā;",
  "ja538:70.3_0": "Kise thūle vivajjetvā,",
  "ja538:70.4_0": "saṁsaṭṭhā yojitā hayā”.",
  "ja538:71.1_0": "Tato rājā taramāno,",
  "ja538:71.2_0": "yuttamāruyha sandanaṁ;",
  "ja538:71.3_0": "Itthāgāraṁ ajjhabhāsi,",
  "ja538:71.4_0": "“sabbāva anuyātha maṁ.",
  "ja538:72.1_0": "Vālabījanimuṇhīsaṁ,",
  "ja538:72.2_0": "khaggaṁ chattañca paṇḍaraṁ;",
  "ja538:72.3_0": "Upādhī rathamāruyha,",
  "ja538:72.4_0": "suvaṇṇehi alaṅkatā”.",
  "ja538:73.1_0": "Tato sa rājā pāyāsi,",
  "ja538:73.2_0": "purakkhatvāna sārathiṁ;",
  "ja538:73.3_0": "Khippameva upāgacchi,",
  "ja538:73.4_0": "yattha sammati temiyo.",
  "ja538:74.1_0": "Tañca disvāna āyantaṁ,",
  "ja538:74.2_0": "jalantamiva tejasā;",
  "ja538:74.3_0": "Khattasaṅghaparibyūḷhaṁ,",
  "ja538:74.4_0": "temiyo etadabravi.",
  "ja538:75.1_0": "“Kacci nu tāta kusalaṁ,",
  "ja538:75.2_0": "kacci tāta anāmayaṁ;",
  "ja538:75.3_0": "Sabbā ca rājakaññāyo,",
  "ja538:75.4_0": "arogā mayha mātaro”.",
  "ja538:76.1_0": "“Kusalañceva me putta,",
  "ja538:76.2_0": "atho putta anāmayaṁ;",
  "ja538:76.3_0": "Sabbā ca rājakaññāyo,",
  "ja538:76.4_0": "arogā tuyha mātaro”.",
  "ja538:77.1_0": "“Kacci amajjapo tāta,",
  "ja538:77.2_0": "kacci te suramappiyaṁ;",
  "ja538:77.3_0": "Kacci sacce ca dhamme ca,",
  "ja538:77.4_0": "dāne te ramate mano”.",
  "ja538:78.1_0": "“Amajjapo ahaṁ putta,",
  "ja538:78.2_0": "atho me suramappiyaṁ;",
  "ja538:78.3_0": "Atho sacce ca dhamme ca,",
  "ja538:78.4_0": "dāne me ramate mano”.",
  "ja538:79.1_0": "“Kacci arogaṁ yoggaṁ te,",
  "ja538:79.2_0": "kacci vahati vāhanaṁ;",
  "ja538:79.3_0": "Kacci te byādhayo natthi,",
  "ja538:79.4_0": "sarīrassupatāpanā”.",
  "ja538:80.1_0": "“Atho arogaṁ yoggaṁ me,",
  "ja538:80.2_0": "atho vahati vāhanaṁ;",
  "ja538:80.3_0": "Atho me byādhayo natthi,",
  "ja538:80.4_0": "sarīrassupatāpanā”.",
  "ja538:81.1_0": "“Kacci antā ca te phītā,",
  "ja538:81.2_0": "majjhe ca bahalā tava;",
  "ja538:81.3_0": "Koṭṭhāgārañca kosañca,",
  "ja538:81.4_0": "kacci te paṭisanthataṁ”.",
  "ja538:82.1_0": "“Atho antā ca me phītā,",
  "ja538:82.2_0": "majjhe ca bahalā mama;",
  "ja538:82.3_0": "Koṭṭhāgārañca kosañca,",
  "ja538:82.4_0": "sabbaṁ me paṭisanthataṁ”.",
  "ja538:83.1_0": "“Svāgataṁ te mahārāja,",
  "ja538:83.2_0": "atho te adurāgataṁ;",
  "ja538:83.3_0": "Patiṭṭhapentu pallaṅkaṁ,",
  "ja538:83.4_0": "yattha rājā nisakkati.",
  "ja538:84.1_0": "Idheva te nisīdassu,",
  "ja538:84.2_0": "niyate paṇṇasanthare;",
  "ja538:84.3_0": "Etto udakamādāya,",
  "ja538:84.4_0": "pāde pakkhālayassu te.",
  "ja538:85.1_0": "Idampi paṇṇakaṁ mayhaṁ,",
  "ja538:85.2_0": "randhaṁ rāja aloṇakaṁ;",
  "ja538:85.3_0": "Paribhuñja mahārāja,",
  "ja538:85.4_0": "pāhuno mesidhāgato”.",
  "ja538:86.1_0": "“Na cāhaṁ paṇṇaṁ bhuñjāmi,",
  "ja538:86.2_0": "Na hetaṁ mayha bhojanaṁ;",
  "ja538:86.3_0": "Sālīnaṁ odanaṁ bhuñje,",
  "ja538:86.4_0": "Suciṁ maṁsūpasecanaṁ”.",
  "ja538:87.1_0": "“Accherakaṁ maṁ paṭibhāti,",
  "ja538:87.2_0": "ekakampi rahogataṁ;",
  "ja538:87.3_0": "Edisaṁ bhuñjamānānaṁ,",
  "ja538:87.4_0": "kena vaṇṇo pasīdati”.",
  "ja538:88.1_0": "“Eko rāja nipajjāmi,",
  "ja538:88.2_0": "niyate paṇṇasanthare;",
  "ja538:88.3_0": "Tāya me ekaseyyāya,",
  "ja538:88.4_0": "rāja vaṇṇo pasīdati.",
  "ja538:89.1_0": "Na ca nettiṁsabandhā me,",
  "ja538:89.2_0": "rājarakkhā upaṭṭhitā;",
  "ja538:89.3_0": "Tāya me sukhaseyyāya,",
  "ja538:89.4_0": "rāja vaṇṇo pasīdati.",
  "ja538:90.1_0": "Atītaṁ nānusocāmi,",
  "ja538:90.2_0": "nappajappāmināgataṁ;",
  "ja538:90.3_0": "Paccuppannena yāpemi,",
  "ja538:90.4_0": "tena vaṇṇo pasīdati.",
  "ja538:91.1_0": "Anāgatappajappāya,",
  "ja538:91.2_0": "atītassānusocanā;",
  "ja538:91.3_0": "Etena bālā sussanti,",
  "ja538:91.4_0": "naḷova harito luto”.",
  "ja538:92.1_0": "“Hatthānīkaṁ rathānīkaṁ,",
  "ja538:92.2_0": "asse pattī ca vammino;",
  "ja538:92.3_0": "Nivesanāni rammāni,",
  "ja538:92.4_0": "ahaṁ putta dadāmi te.",
  "ja538:93.1_0": "Itthāgārampi te dammi,",
  "ja538:93.2_0": "sabbālaṅkārabhūsitaṁ;",
  "ja538:93.3_0": "Tā putta paṭipajjassu,",
  "ja538:93.4_0": "tvaṁ no rājā bhavissasi.",
  "ja538:94.1_0": "Kusalā naccagītassa,",
  "ja538:94.2_0": "sikkhitā cāturitthiyo;",
  "ja538:94.3_0": "Kāme taṁ ramayissanti,",
  "ja538:94.4_0": "kiṁ araññe karissasi.",
  "ja538:95.1_0": "Paṭirājūhi te kaññā,",
  "ja538:95.2_0": "ānayissaṁ alaṅkatā;",
  "ja538:95.3_0": "Tāsu putte janetvāna,",
  "ja538:95.4_0": "atha pacchā pabbajissasi.",
  "ja538:96.1_0": "Yuvā ca daharo cāsi,",
  "ja538:96.2_0": "paṭhamuppattiko susu;",
  "ja538:96.3_0": "Rajjaṁ kārehi bhaddante,",
  "ja538:96.4_0": "kiṁ araññe karissasi”.",
  "ja538:97.1_0": "“Yuvā care brahmacariyaṁ,",
  "ja538:97.2_0": "brahmacārī yuvā siyā;",
  "ja538:97.3_0": "Daharassa hi pabbajjā,",
  "ja538:97.4_0": "etaṁ isīhi vaṇṇitaṁ.",
  "ja538:98.1_0": "Yuvā care brahmacariyaṁ,",
  "ja538:98.2_0": "brahmacārī yuvā siyā;",
  "ja538:98.3_0": "Brahmacariyaṁ carissāmi,",
  "ja538:98.4_0": "nāhaṁ rajjena matthiko.",
  "ja538:99.1_0": "Passāmi vohaṁ daharaṁ,",
  "ja538:99.2_0": "amma tāta vadantaraṁ;",
  "ja538:99.3_0": "Kicchāladdhaṁ piyaṁ puttaṁ,",
  "ja538:99.4_0": "appatvāva jaraṁ mataṁ.",
  "ja538:100.1_0": "Passāmi vohaṁ dahariṁ,",
  "ja538:100.2_0": "kumāriṁ cārudassaniṁ;",
  "ja538:100.3_0": "Navavaṁsakaḷīraṁva,",
  "ja538:100.4_0": "paluggaṁ jīvitakkhayaṁ.",
  "ja538:101.1_0": "Daharāpi hi miyyanti,",
  "ja538:101.2_0": "narā ca atha nāriyo;",
  "ja538:101.3_0": "Tattha ko vissase poso,",
  "ja538:101.4_0": "daharomhīti jīvite.",
  "ja538:102.1_0": "Yassa ratyā vivasāne,",
  "ja538:102.2_0": "āyu appataraṁ siyā;",
  "ja538:102.3_0": "Appodakeva macchānaṁ,",
  "ja538:102.4_0": "kiṁ nu komārakaṁ tahiṁ.",
  "ja538:103.1_0": "Niccamabbhāhato loko,",
  "ja538:103.2_0": "niccañca parivārito;",
  "ja538:103.3_0": "Amoghāsu vajantīsu,",
  "ja538:103.4_0": "kiṁ maṁ rajjebhisiñcasi”.",
  "ja538:104.1_0": "“Kena mabbhāhato loko,",
  "ja538:104.2_0": "kena ca parivārito;",
  "ja538:104.3_0": "Kāyo amoghā gacchanti,",
  "ja538:104.4_0": "taṁ me akkhāhi pucchito”.",
  "ja538:105.1_0": "“Maccunābbhāhato loko,",
  "ja538:105.2_0": "jarāya parivārito;",
  "ja538:105.3_0": "Ratyo amoghā gacchanti,",
  "ja538:105.4_0": "evaṁ jānāhi khattiya.",
  "ja538:106.1_0": "Yathāpi tante vitate,",
  "ja538:106.2_0": "yaṁ yadevūpaviyyati;",
  "ja538:106.3_0": "Appakaṁ hoti vetabbaṁ,",
  "ja538:106.4_0": "evaṁ maccāna jīvitaṁ.",
  "ja538:107.1_0": "Yathā vārivaho pūro,",
  "ja538:107.2_0": "gacchannupanivattati;",
  "ja538:107.3_0": "Evamāyu manussānaṁ,",
  "ja538:107.4_0": "gacchannupanivattati.",
  "ja538:108.1_0": "Yathā vārivaho pūro,",
  "ja538:108.2_0": "vahe rukkhepakūlaje;",
  "ja538:108.3_0": "Evaṁ jarāmaraṇena,",
  "ja538:108.4_0": "vuyhante sabbapāṇino”.",
  "ja538:109.1_0": "“Hatthānīkaṁ rathānīkaṁ,",
  "ja538:109.2_0": "asse pattī ca vammino;",
  "ja538:109.3_0": "Nivesanāni rammāni,",
  "ja538:109.4_0": "ahaṁ putta dadāmi te.",
  "ja538:110.1_0": "Itthāgārampi te dammi,",
  "ja538:110.2_0": "sabbālaṅkārabhūsitaṁ;",
  "ja538:110.3_0": "Tā putta paṭipajjassu,",
  "ja538:110.4_0": "tvaṁ no rājā bhavissasi.",
  "ja538:111.1_0": "Kusalā naccagītassa,",
  "ja538:111.2_0": "sikkhitā cāturitthiyo;",
  "ja538:111.3_0": "Kāme taṁ ramayissanti,",
  "ja538:111.4_0": "kiṁ araññe karissasi.",
  "ja538:112.1_0": "Paṭirājūhi te kaññā,",
  "ja538:112.2_0": "ānayissaṁ alaṅkatā;",
  "ja538:112.3_0": "Tāsu putte janetvāna,",
  "ja538:112.4_0": "atha pacchā pabbajissasi.",
  "ja538:113.1_0": "Yuvā ca daharo cāsi,",
  "ja538:113.2_0": "paṭhamuppattiko susu;",
  "ja538:113.3_0": "Rajjaṁ kārehi bhaddante,",
  "ja538:113.4_0": "kiṁ araññe karissasi.",
  "ja538:114.1_0": "Koṭṭhāgārañca kosañca,",
  "ja538:114.2_0": "vāhanāni balāni ca;",
  "ja538:114.3_0": "Nivesanāni rammāni,",
  "ja538:114.4_0": "ahaṁ putta dadāmi te.",
  "ja538:115.1_0": "Gomaṇḍalaparibyūḷho,",
  "ja538:115.2_0": "dāsisaṅghapurakkhato;",
  "ja538:115.3_0": "Rajjaṁ kārehi bhaddante,",
  "ja538:115.4_0": "kiṁ araññe karissasi”.",
  "ja538:116.1_0": "“Kiṁ dhanena yaṁ khīyetha,",
  "ja538:116.2_0": "Kiṁ bhariyāya marissati;",
  "ja538:116.3_0": "Kiṁ yobbanena jiṇṇena,",
  "ja538:116.4_0": "Yaṁ jarāyābhibhuyyati.",
  "ja538:117.1_0": "Tattha kā nandi kā khiḍḍā,",
  "ja538:117.2_0": "kā rati kā dhanesanā;",
  "ja538:117.3_0": "Kiṁ me puttehi dārehi,",
  "ja538:117.4_0": "rāja muttosmi bandhanā.",
  "ja538:118.1_0": "Yohaṁ evaṁ pajānāmi,",
  "ja538:118.2_0": "maccu me nappamajjati;",
  "ja538:118.3_0": "Antakenādhipannassa,",
  "ja538:118.4_0": "kā ratī kā dhanesanā.",
  "ja538:119.1_0": "Phalānamiva pakkānaṁ,",
  "ja538:119.2_0": "niccaṁ patanato bhayaṁ;",
  "ja538:119.3_0": "Evaṁ jātāna maccānaṁ,",
  "ja538:119.4_0": "niccaṁ maraṇato bhayaṁ.",
  "ja538:120.1_0": "Sāyameke na dissanti,",
  "ja538:120.2_0": "pāto diṭṭhā bahū janā;",
  "ja538:120.3_0": "Pāto eke na dissanti,",
  "ja538:120.4_0": "sāyaṁ diṭṭhā bahū janā.",
  "ja538:121.1_0": "Ajjeva kiccaṁ ātapaṁ,",
  "ja538:121.2_0": "ko jaññā maraṇaṁ suve;",
  "ja538:121.3_0": "Na hi no saṅgaraṁ tena,",
  "ja538:121.4_0": "mahāsenena maccunā.",
  "ja538:122.1_0": "Corā dhanassa patthenti,",
  "ja538:122.2_0": "rāja muttosmi bandhanā;",
  "ja538:122.3_0": "Ehi rāja nivattassu,",
  "ja538:122.4_0": "nāhaṁ rajjena matthiko”ti.",
  "ja538:123.1_0": "Mūgapakkhajātakaṁ paṭhamaṁ."
}