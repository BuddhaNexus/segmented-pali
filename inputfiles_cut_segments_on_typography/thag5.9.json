{
  "thag5.9:0.1_0": "Theragāthā 5.9",
  "thag5.9:0.2_0": "Pañcakanipāta",
  "thag5.9:0.3_0": "Paṭhamavagga",
  "thag5.9:0.4_0": "Vijitasenattheragāthā",
  "thag5.9:1.1_0": "“Olaggessāmi te citta,",
  "thag5.9:1.2_0": "āṇidvāreva hatthinaṁ;",
  "thag5.9:1.3_0": "Na taṁ pāpe niyojessaṁ,",
  "thag5.9:1.4_0": "kāmajāla sarīraja.",
  "thag5.9:2.1_0": "Tvaṁ olaggo na gacchasi,",
  "thag5.9:2.2_0": "Dvāravivaraṁ gajova alabhanto;",
  "thag5.9:2.3_0": "Na ca cittakali punappunaṁ,",
  "thag5.9:2.4_0": "Pasakka pāparato carissasi.",
  "thag5.9:3.1_0": "Yathā kuñjaraṁ adantaṁ,",
  "thag5.9:3.2_0": "Navaggahamaṅkusaggaho;",
  "thag5.9:3.3_0": "Balavā āvatteti akāmaṁ,",
  "thag5.9:3.4_0": "Evaṁ āvattayissaṁ taṁ.",
  "thag5.9:4.1_0": "Yathā varahayadamakusalo,",
  "thag5.9:4.2_0": "Sārathi pavaro dameti ājaññaṁ;",
  "thag5.9:4.3_0": "Evaṁ damayissaṁ taṁ,",
  "thag5.9:4.4_0": "Patiṭṭhito pañcasu balesu.",
  "thag5.9:5.1_0": "Satiyā taṁ nibandhissaṁ,",
  "thag5.9:5.2_0": "Payutto te damessāmi;",
  "thag5.9:5.3_0": "Vīriyadhuraniggahito,",
  "thag5.9:5.4_0": "Na yito dūraṁ gamissase cittā”ti.",
  "thag5.9:5.5_0": "… Vijitaseno thero …."
}