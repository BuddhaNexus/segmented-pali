{
  "tha-ap167:0.1_0": "Therāpadāna",
  "tha-ap167:0.2_0": "Supāricariyavagga",
  "tha-ap167:0.3_0": "5 Kaṇikārachattiyattheraapadāna",
  "tha-ap167:1.1_0": "“Vessabhū nāma sambuddho,",
  "tha-ap167:1.2_0": "lokajeṭṭho narāsabho;",
  "tha-ap167:1.3_0": "Divāvihārāya muni,",
  "tha-ap167:1.4_0": "ogāhayi mahāvanaṁ.",
  "tha-ap167:2.1_0": "Kaṇikāraṁ ocinitvā,",
  "tha-ap167:2.2_0": "chattaṁ katvānahaṁ tadā;",
  "tha-ap167:2.3_0": "Pupphacchadanaṁ katvāna,",
  "tha-ap167:2.4_0": "buddhassa abhiropayiṁ.",
  "tha-ap167:3.1_0": "Ekattiṁse ito kappe,",
  "tha-ap167:3.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap167:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap167:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap167:4.1_0": "Ito vīsatikappamhi,",
  "tha-ap167:4.2_0": "soṇṇābhā aṭṭha khattiyā;",
  "tha-ap167:4.3_0": "Sattaratanasampannā,",
  "tha-ap167:4.4_0": "cakkavattī mahabbalā.",
  "tha-ap167:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap167:5.2_0": "…pe…",
  "tha-ap167:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap167:6.1_0": "Itthaṁ sudaṁ āyasmā kaṇikārachattiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap167:7.1_0": "Kaṇikārachattiyattherassāpadānaṁ pañcamaṁ."
}