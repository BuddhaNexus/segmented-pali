{
  "ja418:0.1_0": "Jātaka",
  "ja418:0.2_0": "Aṭṭhakanipāta",
  "ja418:0.3_0": "Kaccānivagga",
  "ja418:0.4_0": "2. Aṭṭhasaddajātaka",
  "ja418:1.1_0": "“Idaṁ pure ninnamāhu,",
  "ja418:1.2_0": "bahumacchaṁ mahodakaṁ;",
  "ja418:1.3_0": "Āvāso bakarājassa,",
  "ja418:1.4_0": "pettikaṁ bhavanaṁ mama;",
  "ja418:1.5_0": "Tyajja bhekena yāpema,",
  "ja418:1.6_0": "okaṁ na vijahāmase”.",
  "ja418:2.1_0": "“Ko dutiyaṁ asīlissa,",
  "ja418:2.2_0": "bandharassakkhi bhecchati;",
  "ja418:2.3_0": "Ko me putte kulāvakaṁ,",
  "ja418:2.4_0": "mañca sotthiṁ karissati”.",
  "ja418:3.1_0": "“Sabbā parikkhayā pheggu,",
  "ja418:3.2_0": "yāva tassā gatī ahu;",
  "ja418:3.3_0": "Khīṇabhakkho mahārāja,",
  "ja418:3.4_0": "sāre na ramatī ghuṇo”.",
  "ja418:4.1_0": "“Sā nūnāhaṁ ito gantvā,",
  "ja418:4.2_0": "rañño muttā nivesanā;",
  "ja418:4.3_0": "Attānaṁ ramayissāmi,",
  "ja418:4.4_0": "dumasākhaniketinī”.",
  "ja418:5.1_0": "“So nūnāhaṁ ito gantvā,",
  "ja418:5.2_0": "rañño mutto nivesanā;",
  "ja418:5.3_0": "Aggodakāni pissāmi,",
  "ja418:5.4_0": "yūthassa purato vajaṁ”.",
  "ja418:6.1_0": "“Taṁ maṁ kāmehi sammattaṁ,",
  "ja418:6.2_0": "rattaṁ kāmesu mucchitaṁ;",
  "ja418:6.3_0": "Ānayī bharato luddo,",
  "ja418:6.4_0": "bāhiko bhaddamatthu te”.",
  "ja418:7.1_0": "“Andhakāratimisāyaṁ,",
  "ja418:7.2_0": "tuṅge uparipabbate;",
  "ja418:7.3_0": "Sā maṁ saṇhena mudunā,",
  "ja418:7.4_0": "mā pādaṁ khali yasmani”.",
  "ja418:8.1_0": "“Asaṁsayaṁ jātikhayantadassī,",
  "ja418:8.2_0": "Na gabbhaseyyaṁ punarāvajissaṁ;",
  "ja418:8.3_0": "Ayamantimā pacchimā gabbhaseyyā,",
  "ja418:8.4_0": "Khīṇo me saṁsāro punabbhavāyā”ti.",
  "ja418:9.1_0": "Aṭṭhasaddajātakaṁ dutiyaṁ."
}