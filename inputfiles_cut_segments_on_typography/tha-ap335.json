{
  "tha-ap335:0.1_0": "Therāpadāna",
  "tha-ap335:0.2_0": "Gandhodakavagga",
  "tha-ap335:0.3_0": "3. Punnāgapupphiyattheraapadāna",
  "tha-ap335:1.1_0": "“Kānanaṁ vanamogayha,",
  "tha-ap335:1.2_0": "vasāmi luddako ahaṁ;",
  "tha-ap335:1.3_0": "Punnāgaṁ pupphitaṁ disvā,",
  "tha-ap335:1.4_0": "buddhaseṭṭhaṁ anussariṁ.",
  "tha-ap335:2.1_0": "Taṁ pupphaṁ ocinitvāna,",
  "tha-ap335:2.2_0": "sugandhaṁ gandhitaṁ subhaṁ;",
  "tha-ap335:2.3_0": "Thūpaṁ katvāna puline,",
  "tha-ap335:2.4_0": "buddhassa abhiropayiṁ.",
  "tha-ap335:3.1_0": "Dvenavute ito kappe,",
  "tha-ap335:3.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap335:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap335:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap335:4.1_0": "Ekamhi navute kappe,",
  "tha-ap335:4.2_0": "eko āsiṁ tamonudo;",
  "tha-ap335:4.3_0": "Sattaratanasampanno,",
  "tha-ap335:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap335:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap335:5.2_0": "…pe…",
  "tha-ap335:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap335:6.1_0": "Itthaṁ sudaṁ āyasmā punnāgapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap335:7.1_0": "Punnāgapupphiyattherassāpadānaṁ tatiyaṁ."
}