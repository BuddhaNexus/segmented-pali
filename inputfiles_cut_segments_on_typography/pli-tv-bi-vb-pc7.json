{
  "pli-tv-bi-vb-pc7:0.1_0": "Theravāda Vinaya",
  "pli-tv-bi-vb-pc7:0.2_0": "Bhikkhunivibhaṅga",
  "pli-tv-bi-vb-pc7:0.3_0": "Pācittiyakaṇḍa",
  "pli-tv-bi-vb-pc7:0.4_0": "Lasuṇavagga",
  "pli-tv-bi-vb-pc7:0.5_0": "7. Āmakadhaññasikkhāpada",
  "pli-tv-bi-vb-pc7:1.1_0": "Tena samayena buddho bhagavā sāvatthiyaṁ viharati jetavane anāthapiṇḍikassa ārāme.",
  "pli-tv-bi-vb-pc7:1.2_0": "Tena kho pana samayena bhikkhuniyo sassakāle āmakadhaññaṁ viññāpetvā nagaraṁ atiharanti dvāraṭṭhāne—",
  "pli-tv-bi-vb-pc7:1.3_0": "“dethāyye, bhāgan”ti.",
  "pli-tv-bi-vb-pc7:1.4_0": "Palibundhetvā muñciṁsu.",
  "pli-tv-bi-vb-pc7:1.5_0": "Atha kho tā bhikkhuniyo upassayaṁ gantvā bhikkhunīnaṁ etamatthaṁ ārocesuṁ.",
  "pli-tv-bi-vb-pc7:1.6_0": "Yā tā bhikkhuniyo appicchā …pe… tā ujjhāyanti khiyyanti vipācenti—",
  "pli-tv-bi-vb-pc7:1.7_0": "“kathañhi nāma bhikkhuniyo āmakadhaññaṁ viññāpessantī”ti …pe…",
  "pli-tv-bi-vb-pc7:1.8_0": "“saccaṁ kira, bhikkhave, bhikkhuniyo āmakadhaññaṁ viññāpentī”ti?",
  "pli-tv-bi-vb-pc7:1.9_0": "“Saccaṁ, bhagavā”ti.",
  "pli-tv-bi-vb-pc7:1.10_0": "Vigarahi buddho bhagavā …pe…",
  "pli-tv-bi-vb-pc7:1.11_0": "kathañhi nāma, bhikkhave, bhikkhuniyo āmakadhaññaṁ viññāpessanti.",
  "pli-tv-bi-vb-pc7:1.12_0": "Netaṁ, bhikkhave, appasannānaṁ vā pasādāya …pe…",
  "pli-tv-bi-vb-pc7:1.13_0": "evañca pana, bhikkhave, bhikkhuniyo imaṁ sikkhāpadaṁ uddisantu—",
  "pli-tv-bi-vb-pc7:1.14.1_0": "“Yā pana bhikkhunī āmakadhaññaṁ viññatvā vā viññāpetvā vā bhajjitvā vā bhajjāpetvā vā koṭṭetvā vā koṭṭāpetvā vā pacitvā vā pacāpetvā vā bhuñjeyya,",
  "pli-tv-bi-vb-pc7:1.14.1_1": "pācittiyan”ti.",
  "pli-tv-bi-vb-pc7:2.1.1_0": "Yā panāti",
  "pli-tv-bi-vb-pc7:2.1.2_0": "yā yādisā …pe…",
  "pli-tv-bi-vb-pc7:2.1.3_0": "bhikkhunīti",
  "pli-tv-bi-vb-pc7:2.1.4_0": "…pe… ayaṁ imasmiṁ atthe adhippetā bhikkhunīti.",
  "pli-tv-bi-vb-pc7:2.1.5_0": "Āmakadhaññaṁ nāma",
  "pli-tv-bi-vb-pc7:2.1.6_0": "sāli vīhi yavo godhumo kaṅgu varako kudrusako.",
  "pli-tv-bi-vb-pc7:2.1.7_0": "Viññatvāti",
  "pli-tv-bi-vb-pc7:2.1.8_0": "sayaṁ viññatvā.",
  "pli-tv-bi-vb-pc7:2.1.9_0": "Viññāpetvāti",
  "pli-tv-bi-vb-pc7:2.1.10_0": "aññaṁ viññāpetvā.",
  "pli-tv-bi-vb-pc7:2.1.11_0": "Bhajjitvāti",
  "pli-tv-bi-vb-pc7:2.1.12_0": "sayaṁ bhajjitvā.",
  "pli-tv-bi-vb-pc7:2.1.13_0": "Bhajjāpetvāti",
  "pli-tv-bi-vb-pc7:2.1.14_0": "aññaṁ bhajjāpetvā.",
  "pli-tv-bi-vb-pc7:2.1.15_0": "Koṭṭetvāti",
  "pli-tv-bi-vb-pc7:2.1.16_0": "sayaṁ koṭṭetvā.",
  "pli-tv-bi-vb-pc7:2.1.17_0": "Koṭṭāpetvāti",
  "pli-tv-bi-vb-pc7:2.1.18_0": "aññaṁ koṭṭāpetvā.",
  "pli-tv-bi-vb-pc7:2.1.19_0": "Pacitvāti",
  "pli-tv-bi-vb-pc7:2.1.20_0": "sayaṁ pacitvā.",
  "pli-tv-bi-vb-pc7:2.1.21_0": "Pacāpetvāti",
  "pli-tv-bi-vb-pc7:2.1.22_0": "aññaṁ pacāpetvā.",
  "pli-tv-bi-vb-pc7:2.1.23_0": "“Bhuñjissāmī”ti paṭiggaṇhāti, āpatti dukkaṭassa.",
  "pli-tv-bi-vb-pc7:2.1.24_0": "Ajjhohāre ajjhohāre āpatti pācittiyassa.",
  "pli-tv-bi-vb-pc7:2.2.1_0": "Anāpatti—",
  "pli-tv-bi-vb-pc7:2.2.2_0": "ābādhapaccayā,",
  "pli-tv-bi-vb-pc7:2.2.3_0": "aparaṇṇaṁ viññāpeti,",
  "pli-tv-bi-vb-pc7:2.2.4_0": "ummattikāya,",
  "pli-tv-bi-vb-pc7:2.2.5_0": "ādikammikāyāti.",
  "pli-tv-bi-vb-pc7:2.2.6_0": "Sattamasikkhāpadaṁ niṭṭhitaṁ."
}