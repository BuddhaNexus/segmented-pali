{
  "thag1.108:0.1_0": "Theragāthā 1.108",
  "thag1.108:0.2_0": "Ekakanipāta",
  "thag1.108:0.3_0": "Ekādasamavagga",
  "thag1.108:0.4_0": "Dhammasavapituttheragāthā",
  "thag1.108:1.1_0": "“Sa vīsavassasatiko,",
  "thag1.108:1.2_0": "pabbajiṁ anagāriyaṁ;",
  "thag1.108:1.3_0": "Tisso vijjā anuppattā,",
  "thag1.108:1.4_0": "kataṁ buddhassa sāsanan”ti.",
  "thag1.108:1.5_0": "… Dhammasavapitu thero …."
}