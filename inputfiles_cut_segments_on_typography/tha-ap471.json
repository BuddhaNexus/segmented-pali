{
  "tha-ap471:0.1_0": "Therāpadāna",
  "tha-ap471:0.2_0": "Sālakusumiyavagga",
  "tha-ap471:0.3_0": "9 Sayampaṭibhāniyattheraapadāna",
  "tha-ap471:1.1_0": "“Kakudhaṁ vilasantaṁva,",
  "tha-ap471:1.2_0": "devadevaṁ narāsabhaṁ;",
  "tha-ap471:1.3_0": "Rathiyaṁ paṭipajjantaṁ,",
  "tha-ap471:1.4_0": "ko disvā na pasīdati.",
  "tha-ap471:2.1_0": "Tamandhakāraṁ nāsetvā,",
  "tha-ap471:2.2_0": "santāretvā bahuṁ janaṁ;",
  "tha-ap471:2.3_0": "Ñāṇālokena jotantaṁ,",
  "tha-ap471:2.4_0": "ko disvā na pasīdati.",
  "tha-ap471:3.1_0": "Vasīsatasahassehi,",
  "tha-ap471:3.2_0": "nīyantaṁ lokanāyakaṁ;",
  "tha-ap471:3.3_0": "Uddharantaṁ bahū satte,",
  "tha-ap471:3.4_0": "ko disvā na pasīdati.",
  "tha-ap471:4.1_0": "Āhanantaṁ dhammabheriṁ,",
  "tha-ap471:4.2_0": "maddantaṁ titthiye gaṇe;",
  "tha-ap471:4.3_0": "Sīhanādaṁ vinadantaṁ,",
  "tha-ap471:4.4_0": "ko disvā na pasīdati.",
  "tha-ap471:5.1_0": "Yāvatā brahmalokato,",
  "tha-ap471:5.2_0": "āgantvāna sabrahmakā;",
  "tha-ap471:5.3_0": "Pucchanti nipuṇe pañhe,",
  "tha-ap471:5.4_0": "ko disvā na pasīdati.",
  "tha-ap471:6.1_0": "Yassañjaliṁ karitvāna,",
  "tha-ap471:6.2_0": "āyācanti sadevakā;",
  "tha-ap471:6.3_0": "Tena puññaṁ anubhonti,",
  "tha-ap471:6.4_0": "ko disvā na pasīdati.",
  "tha-ap471:7.1_0": "Sabbe janā samāgantvā,",
  "tha-ap471:7.2_0": "sampavārenti cakkhumaṁ;",
  "tha-ap471:7.3_0": "Na vikampati ajjhiṭṭho,",
  "tha-ap471:7.4_0": "ko disvā na pasīdati.",
  "tha-ap471:8.1_0": "Nagaraṁ pavisato yassa,",
  "tha-ap471:8.2_0": "ravanti bheriyo bahū;",
  "tha-ap471:8.3_0": "Vinadanti gajā mattā,",
  "tha-ap471:8.4_0": "ko disvā na pasīdati.",
  "tha-ap471:9.1_0": "Vīthiyā gacchato yassa,",
  "tha-ap471:9.2_0": "sabbābhā jotate sadā;",
  "tha-ap471:9.3_0": "Abbhunnatā samā honti,",
  "tha-ap471:9.4_0": "ko disvā na pasīdati.",
  "tha-ap471:10.1_0": "Byāharantassa buddhassa,",
  "tha-ap471:10.2_0": "cakkavāḷampi suyyati;",
  "tha-ap471:10.3_0": "Sabbe satte viññāpeti,",
  "tha-ap471:10.4_0": "ko disvā na pasīdati.",
  "tha-ap471:11.1_0": "Satasahassito kappe,",
  "tha-ap471:11.2_0": "yaṁ buddhamabhikittayiṁ;",
  "tha-ap471:11.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap471:11.4_0": "kittanāya idaṁ phalaṁ.",
  "tha-ap471:12.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap471:12.2_0": "…pe…",
  "tha-ap471:12.3_0": "viharāmi anāsavo.",
  "tha-ap471:13.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap471:13.2_0": "…pe…",
  "tha-ap471:13.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap471:14.1_0": "Paṭisambhidā catasso,",
  "tha-ap471:14.2_0": "…pe…",
  "tha-ap471:14.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap471:15.1_0": "Itthaṁ sudaṁ āyasmā sayampaṭibhāniyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap471:16.1_0": "Sayampaṭibhāniyattherassāpadānaṁ navamaṁ."
}