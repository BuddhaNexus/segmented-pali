{
  "ja532:0.1_0": "Jātaka",
  "ja532:0.2_0": "Sattatinipāta",
  "ja532:0.3_0": "Kusavagga",
  "ja532:0.4_0": "2. Soṇanandajātaka",
  "ja532:1.1_0": "“Devatā nusi gandhabbo,",
  "ja532:1.2_0": "adu sakko purindado;",
  "ja532:1.3_0": "Manussabhūto iddhimā,",
  "ja532:1.4_0": "kathaṁ jānemu taṁ mayaṁ”.",
  "ja532:2.1_0": "“Nāpi devo na gandhabbo,",
  "ja532:2.2_0": "nāpi sakko purindado;",
  "ja532:2.3_0": "Manussabhūto iddhimā,",
  "ja532:2.4_0": "evaṁ jānāhi bhāradha”.",
  "ja532:3.1_0": "“Katarūpamidaṁ bhoto,",
  "ja532:3.2_0": "veyyāvaccaṁ anappakaṁ;",
  "ja532:3.3_0": "Devamhi vassamānamhi,",
  "ja532:3.4_0": "anovassaṁ bhavaṁ akā.",
  "ja532:4.1_0": "Tato vātātape ghore,",
  "ja532:4.2_0": "sītacchāyaṁ bhavaṁ akā;",
  "ja532:4.3_0": "Tato amittamajjhesu,",
  "ja532:4.4_0": "saratāṇaṁ bhavaṁ akā.",
  "ja532:5.1_0": "Tato phītāni raṭṭhāni,",
  "ja532:5.2_0": "vasino te bhavaṁ akā;",
  "ja532:5.3_0": "Tato ekasataṁ khatye,",
  "ja532:5.4_0": "anuyante bhavaṁ akā.",
  "ja532:6.1_0": "Patītāssu mayaṁ bhoto,",
  "ja532:6.2_0": "Vada taṁ bhañjamicchasi;",
  "ja532:6.3_0": "Hatthiyānaṁ assarathaṁ,",
  "ja532:6.4_0": "Nāriyo ca alaṅkatā;",
  "ja532:6.5_0": "Nivesanāni rammāni,",
  "ja532:6.6_0": "Mayaṁ bhoto dadāmase.",
  "ja532:7.1_0": "Atha vaṅge vā magadhe,",
  "ja532:7.2_0": "mayaṁ bhoto dadāmase;",
  "ja532:7.3_0": "Atha vā assakāvantī,",
  "ja532:7.4_0": "sumanā damma te mayaṁ.",
  "ja532:8.1_0": "Upaḍḍhaṁ vāpi rajjassa,",
  "ja532:8.2_0": "mayaṁ bhoto dadāmase;",
  "ja532:8.3_0": "Sace te attho rajjena,",
  "ja532:8.4_0": "anusāsa yadicchasi”.",
  "ja532:9.1_0": "“Na me atthopi rajjena,",
  "ja532:9.2_0": "nagarena dhanena vā;",
  "ja532:9.3_0": "Athopi janapadena,",
  "ja532:9.4_0": "attho mayhaṁ na vijjati.",
  "ja532:10.1_0": "Bhotova raṭṭhe vijite,",
  "ja532:10.2_0": "araññe atthi assamo;",
  "ja532:10.3_0": "Pitā mayhaṁ janettī ca,",
  "ja532:10.4_0": "ubho sammanti assame.",
  "ja532:11.1_0": "Tesāhaṁ pubbācariyesu,",
  "ja532:11.2_0": "Puññaṁ na labhāmi kātave;",
  "ja532:11.3_0": "Bhavantaṁ ajjhāvaraṁ katvā,",
  "ja532:11.4_0": "Soṇaṁ yācemu saṁvaraṁ”.",
  "ja532:12.1_0": "“Karomi te taṁ vacanaṁ,",
  "ja532:12.2_0": "yaṁ maṁ bhaṇasi brāhmaṇa;",
  "ja532:12.3_0": "Etañca kho no akkhāhi,",
  "ja532:12.4_0": "kīvanto hontu yācakā”.",
  "ja532:13.1_0": "“Parosataṁ jānapadā,",
  "ja532:13.2_0": "mahāsālā ca brāhmaṇā;",
  "ja532:13.3_0": "Ime ca khattiyā sabbe,",
  "ja532:13.4_0": "abhijātā yasassino;",
  "ja532:13.5_0": "Bhavañca rājā manojo,",
  "ja532:13.6_0": "alaṁ hessanti yācakā”.",
  "ja532:14.1_0": "“Hatthī asse ca yojentu,",
  "ja532:14.2_0": "rathaṁ sannayha sārathi;",
  "ja532:14.3_0": "Ābandhanāni gaṇhātha,",
  "ja532:14.4_0": "pādāsussārayaddhaje;",
  "ja532:14.5_0": "Assamaṁ taṁ gamissāmi,",
  "ja532:14.6_0": "yattha sammati kosiyo”.",
  "ja532:15.1_0": "Tato ca rājā pāyāsi,",
  "ja532:15.2_0": "senāya caturaṅginī;",
  "ja532:15.3_0": "Agamā assamaṁ rammaṁ,",
  "ja532:15.4_0": "yattha sammati kosiyo.",
  "ja532:16.1_0": "“Kassa kādambayo kājo,",
  "ja532:16.2_0": "vehāsaṁ caturaṅgulaṁ;",
  "ja532:16.3_0": "Aṁsaṁ asamphusaṁ eti,",
  "ja532:16.4_0": "udahārāya gacchato”.",
  "ja532:17.1_0": "“Ahaṁ soṇo mahārāja,",
  "ja532:17.2_0": "tāpaso sahitabbato;",
  "ja532:17.3_0": "Bharāmi mātāpitaro,",
  "ja532:17.4_0": "rattindivamatandito.",
  "ja532:18.1_0": "Vane phalañca mūlañca,",
  "ja532:18.2_0": "āharitvā disampati;",
  "ja532:18.3_0": "Posemi mātāpitaro,",
  "ja532:18.4_0": "pubbe katamanussaraṁ”.",
  "ja532:19.1_0": "“Icchāma assamaṁ gantuṁ,",
  "ja532:19.2_0": "yattha sammati kosiyo;",
  "ja532:19.3_0": "Maggaṁ no soṇa akkhāhi,",
  "ja532:19.4_0": "yena gacchemu assamaṁ”.",
  "ja532:20.1_0": "“Ayaṁ ekapadī rāja,",
  "ja532:20.2_0": "yenetaṁ meghasannibhaṁ;",
  "ja532:20.3_0": "Koviḷārehi sañchannaṁ,",
  "ja532:20.4_0": "ettha sammati kosiyo”.",
  "ja532:21.1_0": "Idaṁ vatvāna pakkāmi,",
  "ja532:21.2_0": "taramāno mahāisi;",
  "ja532:21.3_0": "Vehāse antalikkhasmiṁ,",
  "ja532:21.4_0": "anusāsitvāna khattiye.",
  "ja532:22.1_0": "Assamaṁ parimajjitvā,",
  "ja532:22.2_0": "paññāpetvāna āsanaṁ;",
  "ja532:22.3_0": "Paṇṇasālaṁ pavisitvā,",
  "ja532:22.4_0": "pitaraṁ paṭibodhayi.",
  "ja532:23.1_0": "“Ime āyanti rājāno,",
  "ja532:23.2_0": "abhijātā yasassino;",
  "ja532:23.3_0": "Assamā nikkhamitvāna,",
  "ja532:23.4_0": "nisīda tvaṁ mahāise”.",
  "ja532:24.1_0": "Tassa taṁ vacanaṁ sutvā,",
  "ja532:24.2_0": "taramāno mahāisi;",
  "ja532:24.3_0": "Assamā nikkhamitvāna,",
  "ja532:24.4_0": "sadvāramhi upāvisi.",
  "ja532:25.1_0": "Tañca disvāna āyantaṁ,",
  "ja532:25.2_0": "jalantaṁriva tejasā;",
  "ja532:25.3_0": "Khatyasaṅghaparibyūḷhaṁ,",
  "ja532:25.4_0": "kosiyo etadabravi.",
  "ja532:26.1_0": "Kassa bherī mudiṅgā ca,",
  "ja532:26.2_0": "saṅkhā paṇavadindimā;",
  "ja532:26.3_0": "Purato paṭipannāni,",
  "ja532:26.4_0": "hāsayantā rathesabhaṁ.",
  "ja532:27.1_0": "Kassa kañcanapaṭṭena,",
  "ja532:27.2_0": "puthunā vijjuvaṇṇinā;",
  "ja532:27.3_0": "Yuvā kalāpasannaddho,",
  "ja532:27.4_0": "ko eti siriyā jalaṁ.",
  "ja532:28.1_0": "Ukkāmukhapahaṭṭhaṁva,",
  "ja532:28.2_0": "khadiraṅgārasannibhaṁ;",
  "ja532:28.3_0": "Mukhañca rucirā bhāti,",
  "ja532:28.4_0": "ko eti siriyā jalaṁ.",
  "ja532:29.1_0": "Kassa paggahitaṁ chattaṁ,",
  "ja532:29.2_0": "sasalākaṁ manoramaṁ;",
  "ja532:29.3_0": "Ādiccaraṁsāvaraṇaṁ,",
  "ja532:29.4_0": "ko eti siriyā jalaṁ.",
  "ja532:30.1_0": "Kassa aṅgaṁ pariggayha,",
  "ja532:30.2_0": "vāḷabījanimuttamaṁ;",
  "ja532:30.3_0": "Caranti varapuññassa,",
  "ja532:30.4_0": "hatthikkhandhena āyato.",
  "ja532:31.1_0": "Kassa setāni chattāni,",
  "ja532:31.2_0": "ājānīyā ca vammitā;",
  "ja532:31.3_0": "Samantā parikīrenti,",
  "ja532:31.4_0": "ko eti siriyā jalaṁ.",
  "ja532:32.1_0": "Kassa ekasataṁ khatyā,",
  "ja532:32.2_0": "anuyantā yasassino;",
  "ja532:32.3_0": "Samantānupariyanti,",
  "ja532:32.4_0": "ko eti siriyā jalaṁ.",
  "ja532:33.1_0": "Hatthi assaratha patti,",
  "ja532:33.2_0": "senā ca caturaṅginī;",
  "ja532:33.3_0": "Samantānupariyanti,",
  "ja532:33.4_0": "ko eti siriyā jalaṁ.",
  "ja532:34.1_0": "Kassesā mahatī senā,",
  "ja532:34.2_0": "piṭṭhito anuvattati;",
  "ja532:34.3_0": "Akkhobhaṇī apariyantā,",
  "ja532:34.4_0": "sāgarasseva ūmiyo.",
  "ja532:35.1_0": "Rājābhirājā manojo,",
  "ja532:35.2_0": "indova jayataṁ pati;",
  "ja532:35.3_0": "Nandassajjhāvaraṁ eti,",
  "ja532:35.4_0": "assamaṁ brahmacārinaṁ.",
  "ja532:36.1_0": "Tassesā mahatī senā,",
  "ja532:36.2_0": "piṭṭhito anuvattati;",
  "ja532:36.3_0": "Akkhobhaṇī apariyantā,",
  "ja532:36.4_0": "sāgarasseva ūmiyo.",
  "ja532:37.1_0": "Anulittā candanena,",
  "ja532:37.2_0": "kāsikuttamadhārino;",
  "ja532:37.3_0": "Sabbe pañjalikā hutvā,",
  "ja532:37.4_0": "isīnaṁ ajjhupāgamuṁ.",
  "ja532:38.1_0": "“Kacci nu bhoto kusalaṁ,",
  "ja532:38.2_0": "kacci bhoto anāmayaṁ;",
  "ja532:38.3_0": "Kacci uñchena yāpetha,",
  "ja532:38.4_0": "kacci mūlaphalā bahū.",
  "ja532:39.1_0": "Kacci ḍaṁsā makasā ca,",
  "ja532:39.2_0": "appameva sarīsapā;",
  "ja532:39.3_0": "Vane vāḷamigākiṇṇe,",
  "ja532:39.4_0": "kacci hiṁsā na vijjati”.",
  "ja532:40.1_0": "“Kusalañceva no rāja,",
  "ja532:40.2_0": "atho rāja anāmayaṁ;",
  "ja532:40.3_0": "Atho uñchena yāpema,",
  "ja532:40.4_0": "atho mūlaphalā bahū.",
  "ja532:41.1_0": "Atho ḍaṁsā makasā ca,",
  "ja532:41.2_0": "appameva sarīsapā;",
  "ja532:41.3_0": "Vane vāḷamigākiṇṇe,",
  "ja532:41.4_0": "hiṁsā mayhaṁ na vijjati.",
  "ja532:42.1_0": "Bahūni vassapūgāni,",
  "ja532:42.2_0": "assame sammataṁ idha;",
  "ja532:42.3_0": "Nābhijānāmi uppannaṁ,",
  "ja532:42.4_0": "ābādhaṁ amanoramaṁ.",
  "ja532:43.1_0": "Svāgataṁ te mahārāja,",
  "ja532:43.2_0": "atho te adurāgataṁ;",
  "ja532:43.3_0": "Issarosi anuppatto,",
  "ja532:43.4_0": "yaṁ idhatthi pavedaya.",
  "ja532:44.1_0": "Tindukāni piyālāni,",
  "ja532:44.2_0": "madhuke kāsumāriyo;",
  "ja532:44.3_0": "Phalāni khuddakappāni,",
  "ja532:44.4_0": "bhuñja rāja varaṁ varaṁ.",
  "ja532:45.1_0": "Idampi pānīyaṁ sītaṁ,",
  "ja532:45.2_0": "ābhataṁ girigabbharā;",
  "ja532:45.3_0": "Tato piva mahārāja,",
  "ja532:45.4_0": "sace tvaṁ abhikaṅkhasi”.",
  "ja532:46.1_0": "“Paṭiggahitaṁ yaṁ dinnaṁ,",
  "ja532:46.2_0": "sabbassa agghiyaṁ kataṁ;",
  "ja532:46.3_0": "Nandassāpi nisāmetha,",
  "ja532:46.4_0": "vacanaṁ so pavakkhati.",
  "ja532:47.1_0": "Ajjhāvaramhā nandassa,",
  "ja532:47.2_0": "bhoto santikamāgatā;",
  "ja532:47.3_0": "Suṇātu bhavaṁ vacanaṁ,",
  "ja532:47.4_0": "nandassa parisāya ca”.",
  "ja532:48.1_0": "“Parosataṁ jānapadā,",
  "ja532:48.2_0": "mahāsālā ca brāhmaṇā;",
  "ja532:48.3_0": "Ime ca khattiyā sabbe,",
  "ja532:48.4_0": "abhijātā yasassino;",
  "ja532:48.5_0": "Bhavañca rājā manojo,",
  "ja532:48.6_0": "anumaññantu me vaco.",
  "ja532:49.1_0": "Ye ca santi samītāro,",
  "ja532:49.2_0": "yakkhāni idha massame;",
  "ja532:49.3_0": "Araññe bhūtabhabyāni,",
  "ja532:49.4_0": "suṇantu vacanaṁ mama.",
  "ja532:50.1_0": "Namo katvāna bhūtānaṁ,",
  "ja532:50.2_0": "isiṁ vakkhāmi subbataṁ;",
  "ja532:50.3_0": "So tyāhaṁ dakkhiṇā bāhu,",
  "ja532:50.4_0": "tava kosiya sammato.",
  "ja532:51.1_0": "Pitaraṁ me janettiñca,",
  "ja532:51.2_0": "bhattukāmassa me sato;",
  "ja532:51.3_0": "Vīra puññamidaṁ ṭhānaṁ,",
  "ja532:51.4_0": "mā maṁ kosiya vāraya.",
  "ja532:52.1_0": "Sabbhi hetaṁ upaññātaṁ,",
  "ja532:52.2_0": "mametaṁ upanissaja;",
  "ja532:52.3_0": "Uṭṭhānapāricariyāya,",
  "ja532:52.4_0": "dīgharattaṁ tayā kataṁ;",
  "ja532:52.5_0": "Mātāpitūsu puññāni,",
  "ja532:52.6_0": "mama lokadado bhava.",
  "ja532:53.1_0": "Tatheva santi manujā,",
  "ja532:53.2_0": "dhamme dhammapadaṁ vidū;",
  "ja532:53.3_0": "Maggo saggassa lokassa,",
  "ja532:53.4_0": "yathā jānāsi tvaṁ ise.",
  "ja532:54.1_0": "Uṭṭhānapāricariyāya,",
  "ja532:54.2_0": "mātāpitusukhāvahaṁ;",
  "ja532:54.3_0": "Taṁ maṁ puññā nivāreti,",
  "ja532:54.4_0": "ariyamaggāvaro naro”.",
  "ja532:55.1_0": "“Suṇantu bhonto vacanaṁ,",
  "ja532:55.2_0": "bhāturajjhāvarā mama;",
  "ja532:55.3_0": "Kulavaṁsaṁ mahārāja,",
  "ja532:55.4_0": "porāṇaṁ parihāpayaṁ;",
  "ja532:55.5_0": "Adhammacārī jeṭṭhesu,",
  "ja532:55.6_0": "nirayaṁ sopapajjati.",
  "ja532:56.1_0": "Ye ca dhammassa kusalā,",
  "ja532:56.2_0": "porāṇassa disampati;",
  "ja532:56.3_0": "Cārittena ca sampannā,",
  "ja532:56.4_0": "na te gacchanti duggatiṁ.",
  "ja532:57.1_0": "Mātā pitā ca bhātā ca,",
  "ja532:57.2_0": "bhaginī ñātibandhavā;",
  "ja532:57.3_0": "Sabbe jeṭṭhassa te bhārā,",
  "ja532:57.4_0": "evaṁ jānāhi bhāradha.",
  "ja532:58.1_0": "Ādiyitvā garuṁ bhāraṁ,",
  "ja532:58.2_0": "nāviko viya ussahe;",
  "ja532:58.3_0": "Dhammañca nappamajjāmi,",
  "ja532:58.4_0": "jeṭṭho casmi rathesabha”.",
  "ja532:59.1_0": "“Adhigamā tame ñāṇaṁ,",
  "ja532:59.2_0": "Jālaṁva jātavedato;",
  "ja532:59.3_0": "Evameva no bhavaṁ dhammaṁ,",
  "ja532:59.4_0": "Kosiyo pavidaṁsayi.",
  "ja532:60.1_0": "Yathā udayamādicco,",
  "ja532:60.2_0": "Vāsudevo pabhaṅkaro;",
  "ja532:60.3_0": "Pāṇīnaṁ pavidaṁseti,",
  "ja532:60.4_0": "Rūpaṁ kalyāṇapāpakaṁ;",
  "ja532:60.5_0": "Evameva no bhavaṁ dhammaṁ,",
  "ja532:60.6_0": "Kosiyo pavidaṁsayi”.",
  "ja532:61.1_0": "“Evaṁ me yācamānassa,",
  "ja532:61.2_0": "añjaliṁ nāvabujjhatha;",
  "ja532:61.3_0": "Tava paddhacaro hessaṁ,",
  "ja532:61.4_0": "vuṭṭhito paricārako”.",
  "ja532:62.1_0": "“Addhā nanda vijānāsi,",
  "ja532:62.2_0": "Saddhammaṁ sabbhi desitaṁ;",
  "ja532:62.3_0": "Ariyo ariyasamācāro,",
  "ja532:62.4_0": "Bāḷhaṁ tvaṁ mama ruccasi.",
  "ja532:63.1_0": "Bhavantaṁ vadāmi bhotiñca,",
  "ja532:63.2_0": "suṇātha vacanaṁ mama;",
  "ja532:63.3_0": "Nāyaṁ bhāro bhāramato,",
  "ja532:63.4_0": "ahu mayhaṁ kudācanaṁ.",
  "ja532:64.1_0": "Taṁ maṁ upaṭṭhitaṁ santaṁ,",
  "ja532:64.2_0": "mātāpitusukhāvahaṁ;",
  "ja532:64.3_0": "Nando ajjhāvaraṁ katvā,",
  "ja532:64.4_0": "upaṭṭhānāya yācati.",
  "ja532:65.1_0": "Yo ve icchati kāmena,",
  "ja532:65.2_0": "santānaṁ brahmacārinaṁ;",
  "ja532:65.3_0": "Nandaṁ vo varatha eko,",
  "ja532:65.4_0": "kaṁ nando upatiṭṭhatu”.",
  "ja532:66.1_0": "“Tayā tāta anuññātā,",
  "ja532:66.2_0": "soṇa taṁ nissitā mayaṁ;",
  "ja532:66.3_0": "Upaghātuṁ labhe nandaṁ,",
  "ja532:66.4_0": "muddhani brahmacārinaṁ.",
  "ja532:67.1_0": "Assatthasseva taruṇaṁ,",
  "ja532:67.2_0": "pavāḷaṁ māluteritaṁ;",
  "ja532:67.3_0": "Cirassaṁ nandaṁ disvāna,",
  "ja532:67.4_0": "hadayaṁ me pavedhati.",
  "ja532:68.1_0": "Yadā suttāpi supine,",
  "ja532:68.2_0": "nandaṁ passāmi āgataṁ;",
  "ja532:68.3_0": "Udaggā sumanā homi,",
  "ja532:68.4_0": "nando no āgato ayaṁ.",
  "ja532:69.1_0": "Yadā ca paṭibujjhitvā,",
  "ja532:69.2_0": "nandaṁ passāmi nāgataṁ;",
  "ja532:69.3_0": "Bhiyyo āvisatī soko,",
  "ja532:69.4_0": "domanassañcanappakaṁ.",
  "ja532:70.1_0": "Sāhaṁ ajja cirassampi,",
  "ja532:70.2_0": "nandaṁ passāmi āgataṁ;",
  "ja532:70.3_0": "Bhattucca mayhañca piyo,",
  "ja532:70.4_0": "nando no pāvisī gharaṁ.",
  "ja532:71.1_0": "Pitupi nando suppiyo,",
  "ja532:71.2_0": "Yaṁ nando nappavase gharā;",
  "ja532:71.3_0": "Labhatū tāta nando taṁ,",
  "ja532:71.4_0": "Maṁ nando upatiṭṭhatu”.",
  "ja532:72.1_0": "“Anukampikā patiṭṭhā ca,",
  "ja532:72.2_0": "pubbe rasadadī ca no;",
  "ja532:72.3_0": "Maggo saggassa lokassa,",
  "ja532:72.4_0": "mātā taṁ varate ise.",
  "ja532:73.1_0": "Pubbe rasadadī gottī,",
  "ja532:73.2_0": "mātā puññūpasaṁhitā;",
  "ja532:73.3_0": "Maggo saggassa lokassa,",
  "ja532:73.4_0": "mātā taṁ varate ise”.",
  "ja532:74.1_0": "“Ākaṅkhamānā puttaphalaṁ,",
  "ja532:74.2_0": "devatāya namassati;",
  "ja532:74.3_0": "Nakkhattāni ca pucchati,",
  "ja532:74.4_0": "utusaṁvaccharāni ca.",
  "ja532:75.1_0": "Tassā utumhi nhātāya,",
  "ja532:75.2_0": "hoti gabbhassa vokkamo;",
  "ja532:75.3_0": "Tena dohaḷinī hoti,",
  "ja532:75.4_0": "suhadā tena vuccati.",
  "ja532:76.1_0": "Saṁvaccharaṁ vā ūnaṁ vā,",
  "ja532:76.2_0": "pariharitvā vijāyati;",
  "ja532:76.3_0": "Tena sā janayantīti,",
  "ja532:76.4_0": "janetti tena vuccati.",
  "ja532:77.1_0": "Thanakhīrena gītena,",
  "ja532:77.2_0": "aṅgapāvuraṇena ca;",
  "ja532:77.3_0": "Rodantaṁ puttaṁ toseti,",
  "ja532:77.4_0": "tosentī tena vuccati.",
  "ja532:78.1_0": "Tato vātātape ghore,",
  "ja532:78.2_0": "mamaṁ katvā udikkhati;",
  "ja532:78.3_0": "Dārakaṁ appajānantaṁ,",
  "ja532:78.4_0": "posentī tena vuccati.",
  "ja532:79.1_0": "Yañca mātudhanaṁ hoti,",
  "ja532:79.2_0": "yañca hoti pituddhanaṁ;",
  "ja532:79.3_0": "Ubhayampetassa gopeti,",
  "ja532:79.4_0": "api puttassa no siyā.",
  "ja532:80.1_0": "Evaṁ putta aduṁ putta,",
  "ja532:80.2_0": "iti mātā vihaññati;",
  "ja532:80.3_0": "Pamattaṁ paradāresu,",
  "ja532:80.4_0": "nisīthe pattayobbane;",
  "ja532:80.5_0": "Sāyaṁ puttaṁ anāyantaṁ,",
  "ja532:80.6_0": "iti mātā vihaññati.",
  "ja532:81.1_0": "Evaṁ kicchā bhato poso,",
  "ja532:81.2_0": "mātu aparicārako;",
  "ja532:81.3_0": "Mātari micchā caritvāna,",
  "ja532:81.4_0": "nirayaṁ sopapajjati.",
  "ja532:82.1_0": "Evaṁ kicchā bhato poso,",
  "ja532:82.2_0": "pitu aparicārako;",
  "ja532:82.3_0": "Pitari micchā caritvāna,",
  "ja532:82.4_0": "nirayaṁ sopapajjati.",
  "ja532:83.1_0": "Dhanāpi dhanakāmānaṁ,",
  "ja532:83.2_0": "Nassati iti me sutaṁ;",
  "ja532:83.3_0": "Mātaraṁ aparicaritvāna,",
  "ja532:83.4_0": "Kicchaṁ vā so nigacchati.",
  "ja532:84.1_0": "Dhanāpi dhanakāmānaṁ,",
  "ja532:84.2_0": "Nassati iti me sutaṁ;",
  "ja532:84.3_0": "Pitaraṁ aparicaritvāna,",
  "ja532:84.4_0": "Kicchaṁ vā so nigacchati.",
  "ja532:85.1_0": "Ānando ca pamodo ca,",
  "ja532:85.2_0": "sadā hasitakīḷitaṁ;",
  "ja532:85.3_0": "Mātaraṁ paricaritvāna,",
  "ja532:85.4_0": "labbhametaṁ vijānato.",
  "ja532:86.1_0": "Ānando ca pamodo ca,",
  "ja532:86.2_0": "sadā hasitakīḷitaṁ;",
  "ja532:86.3_0": "Pitaraṁ paricaritvāna,",
  "ja532:86.4_0": "labbhametaṁ vijānato.",
  "ja532:87.1_0": "Dānañca piyavācā ca,",
  "ja532:87.2_0": "atthacariyā ca yā idha;",
  "ja532:87.3_0": "Samānattatā ca dhammesu,",
  "ja532:87.4_0": "tattha tattha yathārahaṁ;",
  "ja532:87.5_0": "Ete kho saṅgahā loke,",
  "ja532:87.6_0": "rathassāṇīva yāyato.",
  "ja532:88.1_0": "Ete ca saṅgahā nāssu,",
  "ja532:88.2_0": "na mātā puttakāraṇā;",
  "ja532:88.3_0": "Labhetha mānaṁ pūjaṁ vā,",
  "ja532:88.4_0": "pitā vā puttakāraṇā.",
  "ja532:89.1_0": "Yasmā ca saṅgahā ete,",
  "ja532:89.2_0": "sammapekkhanti paṇḍitā;",
  "ja532:89.3_0": "Tasmā mahattaṁ papponti,",
  "ja532:89.4_0": "pāsaṁsā ca bhavanti te.",
  "ja532:90.1_0": "Brahmāti mātāpitaro,",
  "ja532:90.2_0": "pubbācariyāti vuccare;",
  "ja532:90.3_0": "Āhuneyyā ca puttānaṁ,",
  "ja532:90.4_0": "pajāya anukampakā.",
  "ja532:91.1_0": "Tasmā hi ne namasseyya,",
  "ja532:91.2_0": "sakkareyya ca paṇḍito;",
  "ja532:91.3_0": "Annena atho pānena,",
  "ja532:91.4_0": "vatthena sayanena ca;",
  "ja532:91.5_0": "Ucchādanena nhāpanena,",
  "ja532:91.6_0": "pādānaṁ dhovanena ca.",
  "ja532:92.1_0": "Tāya naṁ pāricariyāya,",
  "ja532:92.2_0": "Mātāpitūsu paṇḍitā;",
  "ja532:92.3_0": "Idheva naṁ pasaṁsanti,",
  "ja532:92.4_0": "Pecca sagge pamodatī”ti.",
  "ja532:93.1_0": "Soṇanandajātakaṁ dutiyaṁ.",
  "ja532:94.1_0": "Sattatinipātaṁ niṭṭhitaṁ.",
  "ja532:95.1_0": "Tassuddānaṁ",
  "ja532:96.1_0": "Atha sattatimamhi nipātavare,",
  "ja532:96.2_0": "Sabhāvantu kusāvatirājavaro;",
  "ja532:96.3_0": "Atha soṇasunandavaro ca puna,",
  "ja532:96.4_0": "Abhivāsitasattatimamhi suteti."
}