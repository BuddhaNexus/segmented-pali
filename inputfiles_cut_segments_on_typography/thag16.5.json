{
  "thag16.5:0.1_0": "Theragāthā 16.5",
  "thag16.5:0.2_0": "Vīsatinipāta",
  "thag16.5:0.3_0": "Paṭhamavagga",
  "thag16.5:0.4_0": "Mālukyaputtattheragāthā",
  "thag16.5:1.1_0": "“Rūpaṁ disvā sati muṭṭhā,",
  "thag16.5:1.2_0": "Piyaṁ nimittaṁ manasi karoto;",
  "thag16.5:1.3_0": "Sārattacitto vedeti,",
  "thag16.5:1.4_0": "Tañca ajjhosa tiṭṭhati.",
  "thag16.5:2.1_0": "Tassa vaḍḍhanti vedanā,",
  "thag16.5:2.2_0": "anekā rūpasambhavā;",
  "thag16.5:2.3_0": "Abhijjhā ca vihesā ca,",
  "thag16.5:2.4_0": "cittamassūpahaññati;",
  "thag16.5:2.5_0": "Evamācinato dukkhaṁ,",
  "thag16.5:2.6_0": "ārā nibbāna vuccati.",
  "thag16.5:3.1_0": "Saddaṁ sutvā sati muṭṭhā,",
  "thag16.5:3.2_0": "Piyaṁ nimittaṁ manasi karoto;",
  "thag16.5:3.3_0": "Sārattacitto vedeti,",
  "thag16.5:3.4_0": "Tañca ajjhosa tiṭṭhati.",
  "thag16.5:4.1_0": "Tassa vaḍḍhanti vedanā,",
  "thag16.5:4.2_0": "anekā saddasambhavā;",
  "thag16.5:4.3_0": "Abhijjhā ca vihesā ca,",
  "thag16.5:4.4_0": "cittamassūpahaññati;",
  "thag16.5:4.5_0": "Evamācinato dukkhaṁ,",
  "thag16.5:4.6_0": "ārā nibbāna vuccati.",
  "thag16.5:5.1_0": "Gandhaṁ ghatvā sati muṭṭhā,",
  "thag16.5:5.2_0": "Piyaṁ nimittaṁ manasi karoto;",
  "thag16.5:5.3_0": "Sārattacitto vedeti,",
  "thag16.5:5.4_0": "Tañca ajjhosa tiṭṭhati.",
  "thag16.5:6.1_0": "Tassa vaḍḍhanti vedanā,",
  "thag16.5:6.2_0": "anekā gandhasambhavā;",
  "thag16.5:6.3_0": "Abhijjhā ca vihesā ca,",
  "thag16.5:6.4_0": "cittamassūpahaññati;",
  "thag16.5:6.5_0": "Evamācinato dukkhaṁ,",
  "thag16.5:6.6_0": "ārā nibbāna vuccati.",
  "thag16.5:7.1_0": "Rasaṁ bhotvā sati muṭṭhā,",
  "thag16.5:7.2_0": "Piyaṁ nimittaṁ manasi karoto;",
  "thag16.5:7.3_0": "Sārattacitto vedeti,",
  "thag16.5:7.4_0": "Tañca ajjhosa tiṭṭhati.",
  "thag16.5:8.1_0": "Tassa vaḍḍhanti vedanā,",
  "thag16.5:8.2_0": "anekā rasasambhavā;",
  "thag16.5:8.3_0": "Abhijjhā ca vihesā ca,",
  "thag16.5:8.4_0": "cittamassūpahaññati;",
  "thag16.5:8.5_0": "Evamācinato dukkhaṁ,",
  "thag16.5:8.6_0": "ārā nibbāna vuccati.",
  "thag16.5:9.1_0": "Phassaṁ phussa sati muṭṭhā,",
  "thag16.5:9.2_0": "Piyaṁ nimittaṁ manasi karoto;",
  "thag16.5:9.3_0": "Sārattacitto vedeti,",
  "thag16.5:9.4_0": "Tañca ajjhosa tiṭṭhati.",
  "thag16.5:10.1_0": "Tassa vaḍḍhanti vedanā,",
  "thag16.5:10.2_0": "anekā phassasambhavā;",
  "thag16.5:10.3_0": "Abhijjhā ca vihesā ca,",
  "thag16.5:10.4_0": "cittamassūpahaññati;",
  "thag16.5:10.5_0": "Evamācinato dukkhaṁ,",
  "thag16.5:10.6_0": "ārā nibbāna vuccati.",
  "thag16.5:11.1_0": "Dhammaṁ ñatvā sati muṭṭhā,",
  "thag16.5:11.2_0": "Piyaṁ nimittaṁ manasi karoto;",
  "thag16.5:11.3_0": "Sārattacitto vedeti,",
  "thag16.5:11.4_0": "Tañca ajjhosa tiṭṭhati.",
  "thag16.5:12.1_0": "Tassa vaḍḍhanti vedanā,",
  "thag16.5:12.2_0": "anekā dhammasambhavā;",
  "thag16.5:12.3_0": "Abhijjhā ca vihesā ca,",
  "thag16.5:12.4_0": "cittamassūpahaññati;",
  "thag16.5:12.5_0": "Evamācinato dukkhaṁ,",
  "thag16.5:12.6_0": "ārā nibbāna vuccati.",
  "thag16.5:13.1_0": "Na so rajjati rūpesu,",
  "thag16.5:13.2_0": "rūpaṁ disvā patissato;",
  "thag16.5:13.3_0": "Virattacitto vedeti,",
  "thag16.5:13.4_0": "tañca nājjhosa tiṭṭhati.",
  "thag16.5:14.1_0": "Yathāssa passato rūpaṁ,",
  "thag16.5:14.2_0": "sevato cāpi vedanaṁ;",
  "thag16.5:14.3_0": "Khīyati nopacīyati,",
  "thag16.5:14.4_0": "evaṁ so caratī sato;",
  "thag16.5:14.5_0": "Evaṁ apacinato dukkhaṁ,",
  "thag16.5:14.6_0": "santike nibbāna vuccati.",
  "thag16.5:15.1_0": "Na so rajjati saddesu,",
  "thag16.5:15.2_0": "saddaṁ sutvā patissato;",
  "thag16.5:15.3_0": "Virattacitto vedeti,",
  "thag16.5:15.4_0": "tañca nājjhosa tiṭṭhati.",
  "thag16.5:16.1_0": "Yathāssa suṇato saddaṁ,",
  "thag16.5:16.2_0": "sevato cāpi vedanaṁ;",
  "thag16.5:16.3_0": "Khīyati nopacīyati,",
  "thag16.5:16.4_0": "evaṁ so caratī sato;",
  "thag16.5:16.5_0": "Evaṁ apacinato dukkhaṁ,",
  "thag16.5:16.6_0": "santike nibbāna vuccati.",
  "thag16.5:17.1_0": "Na so rajjati gandhesu,",
  "thag16.5:17.2_0": "gandhaṁ ghatvā patissato;",
  "thag16.5:17.3_0": "Virattacitto vedeti,",
  "thag16.5:17.4_0": "tañca nājjhosa tiṭṭhati.",
  "thag16.5:18.1_0": "Yathāssa ghāyato gandhaṁ,",
  "thag16.5:18.2_0": "sevato cāpi vedanaṁ;",
  "thag16.5:18.3_0": "Khīyati nopacīyati,",
  "thag16.5:18.4_0": "evaṁ so caratī sato;",
  "thag16.5:18.5_0": "Evaṁ apacinato dukkhaṁ,",
  "thag16.5:18.6_0": "santike nibbāna vuccati.",
  "thag16.5:19.1_0": "Na so rajjati rasesu,",
  "thag16.5:19.2_0": "rasaṁ bhotvā patissato;",
  "thag16.5:19.3_0": "Virattacitto vedeti,",
  "thag16.5:19.4_0": "tañca nājjhosa tiṭṭhati.",
  "thag16.5:20.1_0": "Yathāssa sāyarato rasaṁ,",
  "thag16.5:20.2_0": "sevato cāpi vedanaṁ;",
  "thag16.5:20.3_0": "Khīyati nopacīyati,",
  "thag16.5:20.4_0": "evaṁ so caratī sato;",
  "thag16.5:20.5_0": "Evaṁ apacinato dukkhaṁ,",
  "thag16.5:20.6_0": "santike nibbāna vuccati.",
  "thag16.5:21.1_0": "Na so rajjati phassesu,",
  "thag16.5:21.2_0": "phassaṁ phussa patissato;",
  "thag16.5:21.3_0": "Virattacitto vedeti,",
  "thag16.5:21.4_0": "tañca nājjhosa tiṭṭhati.",
  "thag16.5:22.1_0": "Yathāssa phusato phassaṁ,",
  "thag16.5:22.2_0": "sevato cāpi vedanaṁ;",
  "thag16.5:22.3_0": "Khīyati nopacīyati,",
  "thag16.5:22.4_0": "evaṁ so caratī sato;",
  "thag16.5:22.5_0": "Evaṁ apacinato dukkhaṁ,",
  "thag16.5:22.6_0": "santike nibbāna vuccati.",
  "thag16.5:23.1_0": "Na so rajjati dhammesu,",
  "thag16.5:23.2_0": "dhammaṁ ñatvā patissato;",
  "thag16.5:23.3_0": "Virattacitto vedeti,",
  "thag16.5:23.4_0": "tañca nājjhosa tiṭṭhati.",
  "thag16.5:24.1_0": "Yathāssa vijānato dhammaṁ,",
  "thag16.5:24.2_0": "sevato cāpi vedanaṁ;",
  "thag16.5:24.3_0": "Khīyati nopacīyati,",
  "thag16.5:24.4_0": "evaṁ so caratī sato;",
  "thag16.5:24.5_0": "Evaṁ apacinato dukkhaṁ,",
  "thag16.5:24.6_0": "santike nibbāna vuccati”.",
  "thag16.5:24.7_0": "… Mālukyaputto thero …."
}