{
  "iti111:0.1_0": "Itivuttaka 111",
  "iti111:0.2_0": "Catukkanipāta",
  "iti111:0.3_0": "Brāhmaṇadhammayāgavagga",
  "iti111:0.4_0": "Sampannasīlasutta",
  "iti111:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti111:2.1_0": "“Sampannasīlā,",
  "iti111:2.1_1": "bhikkhave,",
  "iti111:2.1_2": "viharatha sampannapātimokkhā;",
  "iti111:2.1_3": "pātimokkhasaṁvarasaṁvutā viharatha ācāragocarasampannā aṇumattesu vajjesu bhayadassāvino;",
  "iti111:2.1_4": "samādāya sikkhatha sikkhāpadesu.",
  "iti111:3.1_0": "Sampannasīlānaṁ vo,",
  "iti111:3.1_1": "bhikkhave,",
  "iti111:3.1_2": "viharataṁ sampannapātimokkhānaṁ pātimokkhasaṁvarasaṁvutānaṁ viharataṁ ācāragocarasampannānaṁ aṇumattesu",
  "iti111:3.1_3": "vajjesu bhayadassāvīnaṁ samādāya sikkhataṁ sikkhāpadesu,",
  "iti111:3.1_4": "kimassa uttari karaṇīyaṁ?",
  "iti111:4.1_0": "Carato cepi,",
  "iti111:4.1_1": "bhikkhave,",
  "iti111:4.1_2": "bhikkhuno abhijjhā,",
  "iti111:4.1_3": "byāpādo vigato hoti,",
  "iti111:4.1_4": "thinamiddhaṁ vigataṁ hoti,",
  "iti111:4.1_5": "uddhaccakukkuccaṁ vigataṁ hoti,",
  "iti111:4.1_6": "vicikicchā pahīnā hoti,",
  "iti111:4.1_7": "āraddhaṁ hoti vīriyaṁ asallīnaṁ,",
  "iti111:4.1_8": "upaṭṭhitā sati asammuṭṭhā,",
  "iti111:4.1_9": "passaddho kāyo asāraddho,",
  "iti111:4.1_10": "samāhitaṁ cittaṁ ekaggaṁ.",
  "iti111:4.2_0": "Carampi,",
  "iti111:4.2_1": "bhikkhave,",
  "iti111:4.2_2": "bhikkhu evaṁbhūto ātāpī ottāpī satataṁ samitaṁ āraddhavīriyo pahitattoti vuccati.",
  "iti111:5.1_0": "Ṭhitassa cepi,",
  "iti111:5.1_1": "bhikkhave,",
  "iti111:5.1_2": "bhikkhuno abhijjhā byāpādo vigato hoti …pe…",
  "iti111:5.1_3": "thinamiddhaṁ …",
  "iti111:5.1_4": "uddhaccakukkuccaṁ …",
  "iti111:5.1_5": "vicikicchā pahīnā hoti,",
  "iti111:5.1_6": "āraddhaṁ hoti vīriyaṁ asallīnaṁ,",
  "iti111:5.1_7": "upaṭṭhitā sati asammuṭṭhā,",
  "iti111:5.1_8": "passaddho kāyo asāraddho,",
  "iti111:5.1_9": "samāhitaṁ cittaṁ ekaggaṁ.",
  "iti111:5.2_0": "Ṭhitopi,",
  "iti111:5.2_1": "bhikkhave,",
  "iti111:5.2_2": "bhikkhu evaṁbhūto ātāpī ottāpī satataṁ samitaṁ āraddhavīriyo pahitattoti vuccati.",
  "iti111:6.1_0": "Nisinnassa cepi,",
  "iti111:6.1_1": "bhikkhave,",
  "iti111:6.1_2": "bhikkhuno abhijjhā byāpādo vigato hoti …pe…",
  "iti111:6.1_3": "thinamiddhaṁ …",
  "iti111:6.1_4": "uddhaccakukkuccaṁ …",
  "iti111:6.1_5": "vicikicchā pahīnā hoti,",
  "iti111:6.1_6": "āraddhaṁ hoti vīriyaṁ asallīnaṁ,",
  "iti111:6.1_7": "upaṭṭhitā sati asammuṭṭhā,",
  "iti111:6.1_8": "passaddho kāyo asāraddho,",
  "iti111:6.1_9": "samāhitaṁ cittaṁ ekaggaṁ.",
  "iti111:6.2_0": "Nisinnopi,",
  "iti111:6.2_1": "bhikkhave,",
  "iti111:6.2_2": "bhikkhu evaṁbhūto ātāpī ottāpī satataṁ samitaṁ āraddhavīriyo pahitattoti vuccati.",
  "iti111:7.1_0": "Sayānassa cepi,",
  "iti111:7.1_1": "bhikkhave,",
  "iti111:7.1_2": "bhikkhuno jāgarassa abhijjhā byāpādo vigato hoti …pe…",
  "iti111:7.1_3": "thinamiddhaṁ …",
  "iti111:7.1_4": "uddhaccakukkuccaṁ …",
  "iti111:7.1_5": "vicikicchā pahīnā hoti,",
  "iti111:7.1_6": "āraddhaṁ hoti vīriyaṁ asallīnaṁ,",
  "iti111:7.1_7": "upaṭṭhitā sati asammuṭṭhā,",
  "iti111:7.1_8": "passaddho kāyo asāraddho,",
  "iti111:7.1_9": "samāhitaṁ cittaṁ ekaggaṁ.",
  "iti111:7.2_0": "Sayānopi,",
  "iti111:7.2_1": "bhikkhave,",
  "iti111:7.2_2": "bhikkhu jāgaro evaṁbhūto ātāpī ottāpī satataṁ samitaṁ āraddhavīriyo pahitattoti vuccatī”ti.",
  "iti111:7.3_0": "Etamatthaṁ bhagavā avoca.",
  "iti111:7.4_0": "Tatthetaṁ iti vuccati:",
  "iti111:8.1_0": "“Yataṁ care yataṁ tiṭṭhe,",
  "iti111:8.2_0": "yataṁ acche yataṁ saye;",
  "iti111:8.3_0": "Yataṁ samiñjaye bhikkhu,",
  "iti111:8.4_0": "yatamenaṁ pasāraye.",
  "iti111:9.1_0": "Uddhaṁ tiriyaṁ apācīnaṁ,",
  "iti111:9.2_0": "yāvatājagatogati;",
  "iti111:9.3_0": "Samavekkhitā ca dhammānaṁ,",
  "iti111:9.4_0": "khandhānaṁ udayabbayaṁ.",
  "iti111:10.1_0": "Evaṁ vihārimātāpiṁ,",
  "iti111:10.2_0": "santavuttimanuddhataṁ;",
  "iti111:10.3_0": "Cetosamathasāmīciṁ,",
  "iti111:10.4_0": "sikkhamānaṁ sadā sataṁ;",
  "iti111:10.5_0": "Satataṁ pahitattoti,",
  "iti111:10.6_0": "āhu bhikkhuṁ tathāvidhan”ti.",
  "iti111:11.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti111:11.2_0": "Dvādasamaṁ."
}