{
  "ja505:0.1_0": "Jātaka",
  "ja505:0.2_0": "Vīsatinipāta",
  "ja505:0.3_0": "Mātaṅgavagga",
  "ja505:0.4_0": "9. Somanassajātaka",
  "ja505:1.1_0": "“Ko taṁ hiṁsati heṭheti,",
  "ja505:1.2_0": "Kiṁ dummano socasi appatīto;",
  "ja505:1.3_0": "Kassajja mātāpitaro rudantu,",
  "ja505:1.4_0": "Kvajja setu nihato pathabyā”.",
  "ja505:2.1_0": "“Tuṭṭhosmi deva tava dassanena,",
  "ja505:2.2_0": "Cirassaṁ passāmi taṁ bhūmipāla;",
  "ja505:2.3_0": "Ahiṁsako reṇumanuppavissa,",
  "ja505:2.4_0": "Puttena te heṭhayitosmi deva”.",
  "ja505:3.1_0": "“Āyantu dovārikā khaggabandhā,",
  "ja505:3.2_0": "Kāsāviyā yantu antepurantaṁ;",
  "ja505:3.3_0": "Hantvāna taṁ somanassaṁ kumāraṁ,",
  "ja505:3.4_0": "Chetvāna sīsaṁ varamāharantu”.",
  "ja505:4.1_0": "“Pesitā rājino dūtā,",
  "ja505:4.2_0": "kumāraṁ etadabravuṁ;",
  "ja505:4.3_0": "Issarena vitiṇṇosi,",
  "ja505:4.4_0": "vadhaṁ pattosi khattiya”.",
  "ja505:5.1_0": "“Sa rājaputto paridevayanto,",
  "ja505:5.2_0": "Dasaṅguliṁ añjaliṁ paggahetvā;",
  "ja505:5.3_0": "Ahampi icchāmi janinda daṭṭhuṁ,",
  "ja505:5.4_0": "Jīvaṁ maṁ netvā paṭidassayetha”.",
  "ja505:6.1_0": "Tassa taṁ vacanaṁ sutvā,",
  "ja505:6.2_0": "rañño puttaṁ adassayuṁ;",
  "ja505:6.3_0": "Putto ca pitaraṁ disvā,",
  "ja505:6.4_0": "dūratovajjhabhāsatha.",
  "ja505:7.1_0": "“Āgacchuṁ dovārikā khaggabandhā,",
  "ja505:7.2_0": "Kāsāviyā hantu mamaṁ janinda;",
  "ja505:7.3_0": "Akkhāhi me pucchito etamatthaṁ,",
  "ja505:7.4_0": "Aparādho ko nidha mamajja atthi”.",
  "ja505:8.1_0": "“Sāyañca pāto udakaṁ sajāti,",
  "ja505:8.2_0": "Aggiṁ sadā pāricaratappamatto;",
  "ja505:8.3_0": "Taṁ tādisaṁ saṁyataṁ brahmacāriṁ,",
  "ja505:8.4_0": "Kasmā tuvaṁ brūsi gahappatīti”.",
  "ja505:9.1_0": "“Tālā ca mūlā ca phalā ca deva,",
  "ja505:9.2_0": "Pariggahā vividhā santimassa;",
  "ja505:9.3_0": "Te rakkhati gopayatappamatto,",
  "ja505:9.4_0": "Tasmā ahaṁ brūmi gahappatīti”.",
  "ja505:10.1_0": "“Saccaṁ kho etaṁ vadasi kumāra,",
  "ja505:10.2_0": "Pariggahā vividhā santimassa;",
  "ja505:10.3_0": "Te rakkhati gopayatappamatto,",
  "ja505:10.4_0": "Sa brāhmaṇo gahapati tena hoti”.",
  "ja505:11.1_0": "“Suṇantu mayhaṁ parisā samāgatā,",
  "ja505:11.2_0": "Sanegamā jānapadā ca sabbe;",
  "ja505:11.3_0": "Bālāyaṁ bālassa vaco nisamma,",
  "ja505:11.4_0": "Ahetunā ghātayate maṁ janindo.",
  "ja505:12.1_0": "Daḷhasmi mūle visaṭe virūḷhe,",
  "ja505:12.2_0": "Dunnikkayo veḷu pasākhajāto;",
  "ja505:12.3_0": "Vandāmi pādāni tava janinda,",
  "ja505:12.4_0": "Anujāna maṁ pabbajissāmi deva”.",
  "ja505:13.1_0": "“Bhuñjassu bhoge vipule kumāra,",
  "ja505:13.2_0": "Sabbañca te issariyaṁ dadāmi;",
  "ja505:13.3_0": "Ajjeva tvaṁ kurūnaṁ hohi rājā,",
  "ja505:13.4_0": "Mā pabbajī pabbajjā hi dukkhā”.",
  "ja505:14.1_0": "“Kinnūdha deva tavamatthi bhogā,",
  "ja505:14.2_0": "Pubbevahaṁ devaloke ramissaṁ;",
  "ja505:14.3_0": "Rūpehi saddehi atho rasehi,",
  "ja505:14.4_0": "Gandhehi phassehi manoramehi.",
  "ja505:15.1_0": "Bhuttā ca me bhogā tidivasmiṁ deva,",
  "ja505:15.2_0": "Parivārito accharānaṁ gaṇena;",
  "ja505:15.3_0": "Tuvañca bālaṁ paraneyyaṁ viditvā,",
  "ja505:15.4_0": "Na tādise rājakule vaseyyaṁ”.",
  "ja505:16.1_0": "“Sacāhaṁ bālo paraneyyo asmi,",
  "ja505:16.2_0": "Ekāparādhaṁ khama putta mayhaṁ;",
  "ja505:16.3_0": "Punapi ce edisakaṁ bhaveyya,",
  "ja505:16.4_0": "Yathāmatiṁ somanassa karohi”.",
  "ja505:17.1_0": "“Anisamma kataṁ kammaṁ,",
  "ja505:17.2_0": "anavatthāya cintitaṁ;",
  "ja505:17.3_0": "Bhesajjasseva vebhaṅgo,",
  "ja505:17.4_0": "vipāko hoti pāpako.",
  "ja505:18.1_0": "Nisamma ca kataṁ kammaṁ,",
  "ja505:18.2_0": "sammāvatthāya cintitaṁ;",
  "ja505:18.3_0": "Bhesajjasseva sampatti,",
  "ja505:18.4_0": "vipāko hoti bhadrako.",
  "ja505:19.1_0": "Alaso gihī kāmabhogī na sādhu,",
  "ja505:19.2_0": "Asaññato pabbajito na sādhu;",
  "ja505:19.3_0": "Rājā na sādhu anisammakārī,",
  "ja505:19.4_0": "Yo paṇḍito kodhano taṁ na sādhu.",
  "ja505:20.1_0": "Nisamma khattiyo kayirā,",
  "ja505:20.2_0": "nānisamma disampati;",
  "ja505:20.3_0": "Nisammakārino rāja,",
  "ja505:20.4_0": "yaso kitti ca vaḍḍhati.",
  "ja505:21.1_0": "Nisamma daṇḍaṁ paṇayeyya issaro,",
  "ja505:21.2_0": "Vegā kataṁ tappati bhūmipāla;",
  "ja505:21.3_0": "Sammāpaṇīdhī ca narassa atthā,",
  "ja505:21.4_0": "Anānutappā te bhavanti pacchā.",
  "ja505:22.1_0": "Anānutappāni hi ye karonti,",
  "ja505:22.2_0": "Vibhajja kammāyatanāni loke;",
  "ja505:22.3_0": "Viññuppasatthāni sukhudrayāni,",
  "ja505:22.4_0": "Bhavanti buddhānumatāni tāni.",
  "ja505:23.1_0": "Āgacchuṁ dovārikā khaggabandhā,",
  "ja505:23.2_0": "Kāsāviyā hantu mamaṁ janinda;",
  "ja505:23.3_0": "Mātuñca aṅkasmimahaṁ nisinno,",
  "ja505:23.4_0": "Ākaḍḍhito sahasā tehi deva.",
  "ja505:24.1_0": "Kaṭukañhi sambādhaṁ sukicchaṁ patto,",
  "ja505:24.2_0": "Madhurampi yaṁ jīvitaṁ laddha rāja;",
  "ja505:24.3_0": "Kicchenahaṁ ajja vadhā pamutto,",
  "ja505:24.4_0": "Pabbajjamevābhimanohamasmi”.",
  "ja505:25.1_0": "“Putto tavāyaṁ taruṇo sudhamme,",
  "ja505:25.2_0": "Anukampako somanasso kumāro;",
  "ja505:25.3_0": "Taṁ yācamāno na labhāmi svajja,",
  "ja505:25.4_0": "Arahasi naṁ yācitave tuvampi”.",
  "ja505:26.1_0": "“Ramassu bhikkhācariyāya putta,",
  "ja505:26.2_0": "Nisamma dhammesu paribbajassu;",
  "ja505:26.3_0": "Sabbesu bhūtesu nidhāya daṇḍaṁ,",
  "ja505:26.4_0": "Anindito brahmamupehi ṭhānaṁ”.",
  "ja505:27.1_0": "“Acchera rūpaṁ vata yādisañca,",
  "ja505:27.2_0": "Dukkhitaṁ maṁ dukkhāpayase sudhamme;",
  "ja505:27.3_0": "Yācassu puttaṁ iti vuccamānā,",
  "ja505:27.4_0": "Bhiyyova ussāhayase kumāraṁ”.",
  "ja505:28.1_0": "“Ye vippamuttā anavajjabhogino,",
  "ja505:28.2_0": "Parinibbutā lokamimaṁ caranti;",
  "ja505:28.3_0": "Tamariyamaggaṁ paṭipajjamānaṁ,",
  "ja505:28.4_0": "Na ussahe vārayituṁ kumāraṁ”.",
  "ja505:29.1_0": "“Addhā have sevitabbā sapaññā,",
  "ja505:29.2_0": "Bahussutā ye bahuṭhānacintino;",
  "ja505:29.3_0": "Yesāyaṁ sutvāna subhāsitāni,",
  "ja505:29.4_0": "Appossukkā vītasokā sudhammā”ti.",
  "ja505:30.1_0": "Somanassajātakaṁ navamaṁ."
}