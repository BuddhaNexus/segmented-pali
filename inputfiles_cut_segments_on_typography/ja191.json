{
  "ja191:0.1_0": "Jātaka",
  "ja191:0.2_0": "Dukanipāta",
  "ja191:0.3_0": "Ruhakavagga",
  "ja191:0.4_0": "1. Ruhakajātaka",
  "ja191:1.1_0": "“Api ruhaka chinnāpi,",
  "ja191:1.2_0": "jiyā sandhīyate puna;",
  "ja191:1.3_0": "Sandhīyassu purāṇiyā,",
  "ja191:1.4_0": "mā kodhassa vasaṁ gami”.",
  "ja191:2.1_0": "“Vijjamānesu vākesu,",
  "ja191:2.2_0": "vijjamānesu kārisu;",
  "ja191:2.3_0": "Aññaṁ jiyaṁ karissāmi,",
  "ja191:2.4_0": "alaññeva purāṇiyā”ti.",
  "ja191:3.1_0": "Ruhakajātakaṁ paṭhamaṁ."
}