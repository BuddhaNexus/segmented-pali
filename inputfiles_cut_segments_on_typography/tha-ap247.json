{
  "tha-ap247:0.1_0": "Therāpadāna",
  "tha-ap247:0.2_0": "Tuvaradāyakavagga",
  "tha-ap247:0.3_0": "5. Kuṭidhūpakattheraapadāna",
  "tha-ap247:1.1_0": "“Siddhatthassa bhagavato,",
  "tha-ap247:1.2_0": "ahosiṁ kuṭigopako;",
  "tha-ap247:1.3_0": "Kālena kālaṁ dhūpesiṁ,",
  "tha-ap247:1.4_0": "pasanno sehi pāṇibhi.",
  "tha-ap247:2.1_0": "Catunnavutito kappe,",
  "tha-ap247:2.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap247:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap247:2.4_0": "dhūpadānassidaṁ phalaṁ.",
  "tha-ap247:3.1_0": "Paṭisambhidā catasso,",
  "tha-ap247:3.2_0": "…pe…",
  "tha-ap247:3.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap247:4.1_0": "Itthaṁ sudaṁ āyasmā kuṭidhūpako thero imā gāthāyo abhāsitthāti.",
  "tha-ap247:5.1_0": "Kuṭidhūpakattherassāpadānaṁ pañcamaṁ."
}