{
  "thag6.11:0.1_0": "Theragāthā 6.11",
  "thag6.11:0.2_0": "Chakkanipāta",
  "thag6.11:0.3_0": "Paṭhamavagga",
  "thag6.11:0.4_0": "Nhātakamunittheragāthā",
  "thag6.11:1.1_0": "“Vātarogābhinīto tvaṁ,",
  "thag6.11:1.2_0": "viharaṁ kānane vane;",
  "thag6.11:1.3_0": "Paviddhagocare lūkhe,",
  "thag6.11:1.4_0": "kathaṁ bhikkhu karissasi”.",
  "thag6.11:2.1_0": "“Pītisukhena vipulena,",
  "thag6.11:2.2_0": "pharitvāna samussayaṁ;",
  "thag6.11:2.3_0": "Lūkhampi abhisambhonto,",
  "thag6.11:2.4_0": "viharissāmi kānane.",
  "thag6.11:3.1_0": "Bhāvento satta bojjhaṅge,",
  "thag6.11:3.2_0": "indriyāni balāni ca;",
  "thag6.11:3.3_0": "Jhānasokhummasampanno,",
  "thag6.11:3.4_0": "viharissaṁ anāsavo.",
  "thag6.11:4.1_0": "Vippamuttaṁ kilesehi,",
  "thag6.11:4.2_0": "suddhacittaṁ anāvilaṁ;",
  "thag6.11:4.3_0": "Abhiṇhaṁ paccavekkhanto,",
  "thag6.11:4.4_0": "viharissaṁ anāsavo.",
  "thag6.11:5.1_0": "Ajjhattañca bahiddhā ca,",
  "thag6.11:5.2_0": "ye me vijjiṁsu āsavā;",
  "thag6.11:5.3_0": "Sabbe asesā ucchinnā,",
  "thag6.11:5.4_0": "na ca uppajjare puna.",
  "thag6.11:6.1_0": "Pañcakkhandhā pariññātā,",
  "thag6.11:6.2_0": "tiṭṭhanti chinnamūlakā;",
  "thag6.11:6.3_0": "Dukkhakkhayo anuppatto,",
  "thag6.11:6.4_0": "natthi dāni punabbhavo”ti.",
  "thag6.11:6.5_0": "… Nhātakamunitthero …."
}