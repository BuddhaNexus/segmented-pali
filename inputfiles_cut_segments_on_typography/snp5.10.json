{
  "snp5.10:0.1_0": "Sutta Nipāta 5.10",
  "snp5.10:0.2_0": "Todeyyamāṇavapucchā",
  "snp5.10:1.1_0": "“Yasmiṁ kāmā na vasanti,",
  "snp5.10:1.2_0": "(iccāyasmā todeyyo)",
  "snp5.10:1.3_0": "Taṇhā yassa na vijjati;",
  "snp5.10:1.4_0": "Kathaṅkathā ca yo tiṇṇo,",
  "snp5.10:1.5_0": "Vimokkho tassa kīdiso”.",
  "snp5.10:2.1_0": "“Yasmiṁ kāmā na vasanti,",
  "snp5.10:2.2_0": "(todeyyāti bhagavā)",
  "snp5.10:2.3_0": "Taṇhā yassa na vijjati;",
  "snp5.10:2.4_0": "Kathaṅkathā ca yo tiṇṇo,",
  "snp5.10:2.5_0": "Vimokkho tassa nāparo”.",
  "snp5.10:3.1_0": "“Nirāsaso so uda āsasāno,",
  "snp5.10:3.2_0": "Paññāṇavā so uda paññakappī;",
  "snp5.10:3.3_0": "Muniṁ ahaṁ sakka yathā vijaññaṁ,",
  "snp5.10:3.4_0": "Taṁ me viyācikkha samantacakkhu”.",
  "snp5.10:4.1_0": "“Nirāsaso so na ca āsasāno,",
  "snp5.10:4.2_0": "Paññāṇavā so na ca paññakappī;",
  "snp5.10:4.3_0": "Evampi todeyya muniṁ vijāna,",
  "snp5.10:4.4_0": "Akiñcanaṁ kāmabhave asattan”ti.",
  "snp5.10:4.5_0": "Todeyyamāṇavapucchā navamā."
}