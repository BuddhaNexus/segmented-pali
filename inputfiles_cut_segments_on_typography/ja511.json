{
  "ja511:0.1_0": "Jātaka",
  "ja511:0.2_0": "Tiṁsanipāta",
  "ja511:0.3_0": "Kiṁchandavagga",
  "ja511:0.4_0": "1. Kiṁchandajātaka",
  "ja511:1.1_0": "“Kiṁchando kimadhippāyo,",
  "ja511:1.2_0": "eko sammasi ghammani;",
  "ja511:1.3_0": "Kiṁ patthayāno kiṁ esaṁ,",
  "ja511:1.4_0": "kena atthena brāhmaṇa”.",
  "ja511:2.1_0": "“Yathā mahā vāridharo,",
  "ja511:2.2_0": "kumbho supariṇāhavā;",
  "ja511:2.3_0": "Tathūpamaṁ ambapakkaṁ,",
  "ja511:2.4_0": "vaṇṇagandharasuttamaṁ.",
  "ja511:3.1_0": "Taṁ vuyhamānaṁ sotena,",
  "ja511:3.2_0": "disvānāmalamajjhime;",
  "ja511:3.3_0": "Pāṇīhi naṁ gahetvāna,",
  "ja511:3.4_0": "agyāyatanamāhariṁ.",
  "ja511:4.1_0": "Tato kadalipattesu,",
  "ja511:4.2_0": "nikkhipitvā sayaṁ ahaṁ;",
  "ja511:4.3_0": "Satthena naṁ vikappetvā,",
  "ja511:4.4_0": "khuppipāsaṁ ahāsi me.",
  "ja511:5.1_0": "Sohaṁ apetadaratho,",
  "ja511:5.2_0": "Byantībhūto dukhakkhamo;",
  "ja511:5.3_0": "Assādaṁ nādhigacchāmi,",
  "ja511:5.4_0": "Phalesvaññesu kesuci.",
  "ja511:6.1_0": "Sosetvā nūna maraṇaṁ,",
  "ja511:6.2_0": "taṁ mamaṁ āvahissati;",
  "ja511:6.3_0": "Ambaṁ yassa phalaṁ sādu,",
  "ja511:6.4_0": "madhuraggaṁ manoramaṁ;",
  "ja511:6.5_0": "Yamuddhariṁ vuyhamānaṁ,",
  "ja511:6.6_0": "udadhismā mahaṇṇave.",
  "ja511:7.1_0": "Akkhātaṁ te mayā sabbaṁ,",
  "ja511:7.2_0": "yasmā upavasāmahaṁ;",
  "ja511:7.3_0": "Rammaṁ pati nisinnosmi,",
  "ja511:7.4_0": "puthulomāyutā puthu.",
  "ja511:8.1_0": "Tvañca kho meva akkhāhi,",
  "ja511:8.2_0": "attānamapalāyini;",
  "ja511:8.3_0": "Kā vā tvamasi kalyāṇi,",
  "ja511:8.4_0": "kissa vā tvaṁ sumajjhime.",
  "ja511:9.1_0": "Ruppapaṭṭapalimaṭṭhīva,",
  "ja511:9.2_0": "byagghīva girisānujā;",
  "ja511:9.3_0": "Yā santi nāriyo devesu,",
  "ja511:9.4_0": "devānaṁ paricārikā.",
  "ja511:10.1_0": "Yā ca manussalokasmiṁ,",
  "ja511:10.2_0": "Rūpenānvāgatitthiyo;",
  "ja511:10.3_0": "Rūpena te sadisī natthi,",
  "ja511:10.4_0": "Devesu gandhabbamanussaloke;",
  "ja511:10.5_0": "Puṭṭhāsi me cārupubbaṅgi,",
  "ja511:10.6_0": "Brūhi nāmañca bandhave”.",
  "ja511:11.1_0": "“Yaṁ tvaṁ pati nisinnosi,",
  "ja511:11.2_0": "rammaṁ brāhmaṇa kosikiṁ;",
  "ja511:11.3_0": "Sāhaṁ bhusālayāvutthā,",
  "ja511:11.4_0": "varavārivahoghasā.",
  "ja511:12.1_0": "Nānādumagaṇākiṇṇā,",
  "ja511:12.2_0": "bahukā girikandarā;",
  "ja511:12.3_0": "Mameva pamukhā honti,",
  "ja511:12.4_0": "abhisandanti pāvuse.",
  "ja511:13.1_0": "Atho bahū vanatodā,",
  "ja511:13.2_0": "nīlavārivahindharā;",
  "ja511:13.3_0": "Bahukā nāgavittodā,",
  "ja511:13.4_0": "abhisandanti vārinā.",
  "ja511:14.1_0": "Tā ambajambulabujā,",
  "ja511:14.2_0": "nīpā tālā cudumbarā;",
  "ja511:14.3_0": "Bahūni phalajātāni,",
  "ja511:14.4_0": "āvahanti abhiṇhaso.",
  "ja511:15.1_0": "Yaṁ kiñci ubhato tīre,",
  "ja511:15.2_0": "phalaṁ patati ambuni;",
  "ja511:15.3_0": "Asaṁsayaṁ taṁ sotassa,",
  "ja511:15.4_0": "phalaṁ hoti vasānugaṁ.",
  "ja511:16.1_0": "Etadaññāya medhāvi,",
  "ja511:16.2_0": "puthupañña suṇohi me;",
  "ja511:16.3_0": "Mā rocaya mabhisaṅgaṁ,",
  "ja511:16.4_0": "paṭisedha janādhipa.",
  "ja511:17.1_0": "Na vāhaṁ vaḍḍhavaṁ maññe,",
  "ja511:17.2_0": "Yaṁ tvaṁ raṭṭhābhivaḍḍhana;",
  "ja511:17.3_0": "Āceyyamāno rājisi,",
  "ja511:17.4_0": "Maraṇaṁ abhikaṅkhasi.",
  "ja511:18.1_0": "Tassa jānanti pitaro,",
  "ja511:18.2_0": "gandhabbā ca sadevakā;",
  "ja511:18.3_0": "Ye cāpi isayo loke,",
  "ja511:18.4_0": "saññatattā tapassino;",
  "ja511:18.5_0": "Asaṁsayaṁ tepi jānanti,",
  "ja511:18.6_0": "paṭṭhabhūtā yasassino”.",
  "ja511:19.1_0": "“Evaṁ viditvā vidū sabbadhammaṁ,",
  "ja511:19.2_0": "Viddhaṁsanaṁ cavanaṁ jīvitassa;",
  "ja511:19.3_0": "Na cīyatī tassa narassa pāpaṁ,",
  "ja511:19.4_0": "Sace na ceteti vadhāya tassa.",
  "ja511:20.1_0": "Isipūgasamaññāte,",
  "ja511:20.2_0": "evaṁ lokyā viditā sati;",
  "ja511:20.3_0": "Anariyaparisambhāse,",
  "ja511:20.4_0": "pāpakammaṁ jigīsasi.",
  "ja511:21.1_0": "Sace ahaṁ marissāmi,",
  "ja511:21.2_0": "tīre te puthusussoṇi;",
  "ja511:21.3_0": "Asaṁsayaṁ taṁ asiloko,",
  "ja511:21.4_0": "mayi pete āgamissati.",
  "ja511:22.1_0": "Tasmā hi pāpakaṁ kammaṁ,",
  "ja511:22.2_0": "rakkhasseva sumajjhime;",
  "ja511:22.3_0": "Mā taṁ sabbo jano pacchā,",
  "ja511:22.4_0": "pakuṭṭhāyi mayi mate”.",
  "ja511:23.1_0": "“Aññātametaṁ avisayhasāhi,",
  "ja511:23.2_0": "Attānamambañca dadāmi te taṁ;",
  "ja511:23.3_0": "So duccaje kāmaguṇe pahāya,",
  "ja511:23.4_0": "Santiñca dhammañca adhiṭṭhitosi.",
  "ja511:24.1_0": "Yo hitvā pubbasaññogaṁ,",
  "ja511:24.2_0": "pacchā saṁyojane ṭhito;",
  "ja511:24.3_0": "Adhammañceva carati,",
  "ja511:24.4_0": "pāpañcassa pavaḍḍhati.",
  "ja511:25.1_0": "Ehi taṁ pāpayissāmi,",
  "ja511:25.2_0": "kāmaṁ appossuko bhava;",
  "ja511:25.3_0": "Upānayāmi sītasmiṁ,",
  "ja511:25.4_0": "viharāhi anussuko.",
  "ja511:26.1_0": "Taṁ puppharasamattebhi,",
  "ja511:26.2_0": "vakkaṅgehi arindama;",
  "ja511:26.3_0": "Koñcā mayūrā diviyā,",
  "ja511:26.4_0": "kolaṭṭhimadhusāḷikā;",
  "ja511:26.5_0": "Kūjitā haṁsapūgehi,",
  "ja511:26.6_0": "kokilettha pabodhare.",
  "ja511:27.1_0": "Ambettha vippasākhaggā,",
  "ja511:27.2_0": "palālakhalasannibhā;",
  "ja511:27.3_0": "Kosambasaḷalā nīpā,",
  "ja511:27.4_0": "pakkatālavilambino”.",
  "ja511:28.1_0": "“Mālī tiriṭī kāyūrī,",
  "ja511:28.2_0": "aṅgadī candanussado;",
  "ja511:28.3_0": "Rattiṁ tvaṁ paricāresi,",
  "ja511:28.4_0": "divā vedesi vedanaṁ.",
  "ja511:29.1_0": "Soḷasitthisahassāni,",
  "ja511:29.2_0": "yā temā paricārikā;",
  "ja511:29.3_0": "Evaṁ mahānubhāvosi,",
  "ja511:29.4_0": "abbhuto lomahaṁsano.",
  "ja511:30.1_0": "Kiṁ kammamakarī pubbe,",
  "ja511:30.2_0": "pāpaṁ attadukhāvahaṁ;",
  "ja511:30.3_0": "Yaṁ karitvā manussesu,",
  "ja511:30.4_0": "piṭṭhimaṁsāni khādasi”.",
  "ja511:31.1_0": "“Ajjhenāni paṭiggayha,",
  "ja511:31.2_0": "kāmesu gadhito ahaṁ;",
  "ja511:31.3_0": "Acariṁ dīghamaddhānaṁ,",
  "ja511:31.4_0": "paresaṁ ahitāyahaṁ.",
  "ja511:32.1_0": "Yo piṭṭhimaṁsiko hoti,",
  "ja511:32.2_0": "evaṁ ukkacca khādati;",
  "ja511:32.3_0": "Yathāhaṁ ajja khādāmi,",
  "ja511:32.4_0": "piṭṭhimaṁsāni attano”ti.",
  "ja511:33.1_0": "Kiṁchandajātakaṁ paṭhamaṁ."
}