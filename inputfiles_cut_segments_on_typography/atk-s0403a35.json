{
  "atk-s0403a35:0_0": "10. Ānisaṁsavaggo",
  "atk-s0403a35:1_0": "1-2. Pātubhāvasuttādivaṇṇanā",
  "atk-s0403a35:2_0": "96- 97.",
  "atk-s0403a35:2_1": "Dasamassa paṭhame ariyāyataneti majjhimadese.",
  "atk-s0403a35:2_2": "Indriyānanti manacchaṭṭhānaṁ.",
  "atk-s0403a35:2_3": "Dutiye saddhammaniyatoti sāsanasaddhamme niyato.",
  "atk-s0403a35:2_4": "Asādhāraṇenāti puthujjanehi asādhāraṇena.",
  "atk-s0403a35:3_0": "7. Anavatthitasuttavaṇṇanā",
  "atk-s0403a35:4_0": "102.",
  "atk-s0403a35:4_1": "Sattame anodhiṁ karitvāti “ettakāva saṅkhārā aniccā,",
  "atk-s0403a35:4_2": "na ito pare”ti evaṁ sīmaṁ mariyādaṁ akatvā.",
  "atk-s0403a35:4_3": "Anavatthitāti avatthitāya rahitā,",
  "atk-s0403a35:4_4": "bhijjamānāva hutvā upaṭṭhahissantīti attho.",
  "atk-s0403a35:4_5": "Sabbaloketi sakale tedhātuke.",
  "atk-s0403a35:4_6": "Sāmaññenāti samaṇabhāvena,",
  "atk-s0403a35:4_7": "ariyamaggenāti attho.",
  "atk-s0403a35:5_0": "8. Ukkhittāsikasuttavaṇṇanā",
  "atk-s0403a35:6_0": "103.",
  "atk-s0403a35:6_1": "Aṭṭhame mettāvatāyāti mettāyuttāya pāricariyāya.",
  "atk-s0403a35:6_2": "Satta hi sekhā tathāgataṁ mettāvatāya paricaranti,",
  "atk-s0403a35:6_3": "khīṇāsavo pariciṇṇasatthuko.",
  "atk-s0403a35:7_0": "9. Atammayasuttavaṇṇanā",
  "atk-s0403a35:8_0": "104.",
  "atk-s0403a35:8_1": "Navame atammayoti tammayā vuccanti taṇhādiṭṭhiyo,",
  "atk-s0403a35:8_2": "tāhi rahito.",
  "atk-s0403a35:8_3": "Ahaṁkārāti ahaṁkāradiṭṭhi.",
  "atk-s0403a35:8_4": "Mamaṁkārāti mamaṁkārataṇhā.",
  "atk-s0403a35:8_5": "Sesaṁ sabbattha uttānamevāti.",
  "atk-s0403a35:9_0": "Ānisaṁsavaggo dasamo.",
  "atk-s0403a35:10_0": "Dutiyapaṇṇāsakaṁ niṭṭhitaṁ."
}