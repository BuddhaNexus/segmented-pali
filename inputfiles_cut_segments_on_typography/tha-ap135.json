{
  "tha-ap135:0.1_0": "Therāpadāna",
  "tha-ap135:0.2_0": "Sobhitavagga",
  "tha-ap135:0.3_0": "3 Candanapūjanakattheraapadāna",
  "tha-ap135:1.1_0": "“Candabhāgānadītīre,",
  "tha-ap135:1.2_0": "ahosiṁ kinnaro tadā;",
  "tha-ap135:1.3_0": "Pupphabhakkho cahaṁ āsiṁ,",
  "tha-ap135:1.4_0": "pupphānivasano tathā.",
  "tha-ap135:2.1_0": "Atthadassī tu bhagavā,",
  "tha-ap135:2.2_0": "lokajeṭṭho narāsabho;",
  "tha-ap135:2.3_0": "Vipinaggena niyyāsi,",
  "tha-ap135:2.4_0": "haṁsarājāva ambare.",
  "tha-ap135:3.1_0": "Namo te purisājañña,",
  "tha-ap135:3.2_0": "cittaṁ te suvisodhitaṁ;",
  "tha-ap135:3.3_0": "Pasannamukhavaṇṇosi,",
  "tha-ap135:3.4_0": "vippasannamukhindriyo.",
  "tha-ap135:4.1_0": "Orohitvāna ākāsā,",
  "tha-ap135:4.2_0": "bhūripañño sumedhaso;",
  "tha-ap135:4.3_0": "Saṅghāṭiṁ pattharitvāna,",
  "tha-ap135:4.4_0": "pallaṅkena upāvisi.",
  "tha-ap135:5.1_0": "Vilīnaṁ candanādāya,",
  "tha-ap135:5.2_0": "agamāsiṁ jinantikaṁ;",
  "tha-ap135:5.3_0": "Pasannacitto sumano,",
  "tha-ap135:5.4_0": "buddhassa abhiropayiṁ.",
  "tha-ap135:6.1_0": "Abhivādetvāna sambuddhaṁ,",
  "tha-ap135:6.2_0": "lokajeṭṭhaṁ narāsabhaṁ;",
  "tha-ap135:6.3_0": "Pāmojjaṁ janayitvāna,",
  "tha-ap135:6.4_0": "pakkāmiṁ uttarāmukho.",
  "tha-ap135:7.1_0": "Aṭṭhārase kappasate,",
  "tha-ap135:7.2_0": "candanaṁ yaṁ apūjayiṁ;",
  "tha-ap135:7.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap135:7.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap135:8.1_0": "Catuddase kappasate,",
  "tha-ap135:8.2_0": "ito āsiṁsu te tayo;",
  "tha-ap135:8.3_0": "Rohaṇī nāma nāmena,",
  "tha-ap135:8.4_0": "cakkavattī mahabbalā.",
  "tha-ap135:9.1_0": "Paṭisambhidā catasso,",
  "tha-ap135:9.2_0": "…pe…",
  "tha-ap135:9.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap135:10.1_0": "Itthaṁ sudaṁ āyasmā candanapūjanako thero imā gāthāyo abhāsitthāti.",
  "tha-ap135:11.1_0": "Candanapūjanakattherassāpadānaṁ tatiyaṁ.",
  "tha-ap135:11.2_0": "Aṭṭhamabhāṇavāraṁ."
}