{
  "tha-ap440:0.1_0": "Therāpadāna",
  "tha-ap440:0.2_0": "Ekavihārivagga",
  "tha-ap440:0.3_0": "8. Harītakadāyakattheraapadāna",
  "tha-ap440:1.1_0": "“Harītakaṁ āmalakaṁ,",
  "tha-ap440:1.2_0": "ambajambuvibhītakaṁ;",
  "tha-ap440:1.3_0": "Kolaṁ bhallātakaṁ billaṁ,",
  "tha-ap440:1.4_0": "sayameva harāmahaṁ.",
  "tha-ap440:2.1_0": "Disvāna pabbhāragataṁ,",
  "tha-ap440:2.2_0": "jhāyiṁ jhānarataṁ muniṁ;",
  "tha-ap440:2.3_0": "Ābādhena āpīḷentaṁ,",
  "tha-ap440:2.4_0": "adutīyaṁ mahāmuniṁ.",
  "tha-ap440:3.1_0": "Harītakaṁ gahetvāna,",
  "tha-ap440:3.2_0": "sayambhussa adāsahaṁ;",
  "tha-ap440:3.3_0": "Khādamattamhi bhesajje,",
  "tha-ap440:3.4_0": "byādhi passambhi tāvade.",
  "tha-ap440:4.1_0": "Pahīnadaratho buddho,",
  "tha-ap440:4.2_0": "anumodamakāsi me;",
  "tha-ap440:4.3_0": "‘Bhesajjadāneniminā,",
  "tha-ap440:4.4_0": "byādhivūpasamena ca.",
  "tha-ap440:5.1_0": "Devabhūto manusso vā,",
  "tha-ap440:5.2_0": "jāto vā aññajātiyā;",
  "tha-ap440:5.3_0": "Sabbattha sukhito hotu,",
  "tha-ap440:5.4_0": "mā ca te byādhimāgamā’.",
  "tha-ap440:6.1_0": "Idaṁ vatvāna sambuddho,",
  "tha-ap440:6.2_0": "sayambhū aparājito;",
  "tha-ap440:6.3_0": "Nabhaṁ abbhuggamī dhīro,",
  "tha-ap440:6.4_0": "haṁsarājāva ambare.",
  "tha-ap440:7.1_0": "Yato harītakaṁ dinnaṁ,",
  "tha-ap440:7.2_0": "sayambhussa mahesino;",
  "tha-ap440:7.3_0": "Imaṁ jātiṁ upādāya,",
  "tha-ap440:7.4_0": "byādhi me nupapajjatha.",
  "tha-ap440:8.1_0": "Ayaṁ pacchimako mayhaṁ,",
  "tha-ap440:8.2_0": "carimo vattate bhavo;",
  "tha-ap440:8.3_0": "Tisso vijjā sacchikatā,",
  "tha-ap440:8.4_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap440:9.1_0": "Catunnavutito kappe,",
  "tha-ap440:9.2_0": "bhesajjamadadiṁ tadā;",
  "tha-ap440:9.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap440:9.4_0": "bhesajjassa idaṁ phalaṁ.",
  "tha-ap440:10.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap440:10.2_0": "…pe…",
  "tha-ap440:10.3_0": "viharāmi anāsavo.",
  "tha-ap440:11.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap440:11.2_0": "…pe…",
  "tha-ap440:11.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap440:12.1_0": "Paṭisambhidā catasso,",
  "tha-ap440:12.2_0": "…pe…",
  "tha-ap440:12.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap440:13.1_0": "Itthaṁ sudaṁ āyasmā harītakadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap440:14.1_0": "Harītakadāyakattherassāpadānaṁ aṭṭhamaṁ."
}