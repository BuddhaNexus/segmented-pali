{
  "ja393:0.1_0": "Jātaka",
  "ja393:0.2_0": "Chakkanipāta",
  "ja393:0.3_0": "Kharaputtavagga",
  "ja393:0.4_0": "8. Vighāsādajātaka",
  "ja393:1.1_0": "“Susukhaṁ vata jīvanti,",
  "ja393:1.2_0": "ye janā vighāsādino;",
  "ja393:1.3_0": "Diṭṭheva dhamme pāsaṁsā,",
  "ja393:1.4_0": "samparāye ca suggatī”.",
  "ja393:2.1_0": "“Sukassa bhāsamānassa,",
  "ja393:2.2_0": "na nisāmetha paṇḍitā;",
  "ja393:2.3_0": "Idaṁ suṇātha sodariyā,",
  "ja393:2.4_0": "amhevāyaṁ pasaṁsati”.",
  "ja393:3.1_0": "“Nāhaṁ tumhe pasaṁsāmi,",
  "ja393:3.2_0": "kuṇapādā suṇātha me;",
  "ja393:3.3_0": "Ucchiṭṭhabhojino tumhe,",
  "ja393:3.4_0": "na tumhe vighāsādino”.",
  "ja393:4.1_0": "“Sattavassā pabbajitā,",
  "ja393:4.2_0": "mejjhāraññe sikhaṇḍino;",
  "ja393:4.3_0": "Vighāseneva yāpentā,",
  "ja393:4.4_0": "mayañce bhoto gārayhā;",
  "ja393:4.5_0": "Ke nu bhoto pasaṁsiyā”.",
  "ja393:5.1_0": "“Tumhe sīhānaṁ byagghānaṁ,",
  "ja393:5.2_0": "vāḷānañcāvasiṭṭhakaṁ;",
  "ja393:5.3_0": "Ucchiṭṭheneva yāpentā,",
  "ja393:5.4_0": "maññivho vighāsādino.",
  "ja393:6.1_0": "Ye brāhmaṇassa samaṇassa,",
  "ja393:6.2_0": "aññassa vā vanibbino;",
  "ja393:6.3_0": "Datvāva sesaṁ bhuñjanti,",
  "ja393:6.4_0": "te janā vighāsādino”ti.",
  "ja393:7.1_0": "Vighāsādajātakaṁ aṭṭhamaṁ."
}