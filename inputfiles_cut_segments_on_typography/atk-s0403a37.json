{
  "atk-s0403a37:0_0": "12. Sāmaññavaggavaṇṇanā",
  "atk-s0403a37:1_0": "119- 121.",
  "atk-s0403a37:1_1": "Ito paresu tapussoti dvevācikupāsako.",
  "atk-s0403a37:1_2": "Tathāgate niṭṭhaṅgatoti buddhaguṇesu patiṭṭhitacitto pahīnakaṅkho.",
  "atk-s0403a37:1_3": "Amataṁ addasāti amataddaso.",
  "atk-s0403a37:1_4": "Ariyenāti niddosena lokuttarasīlena.",
  "atk-s0403a37:1_5": "Ñāṇenāti paccavekkhaṇañāṇena.",
  "atk-s0403a37:1_6": "Vimuttiyāti sekhaphalavimuttiyā.",
  "atk-s0403a37:1_7": "Tavakaṇṇikoti evaṁnāmako gahapati.",
  "atk-s0403a37:1_8": "Tapakaṇṇikotipi pāḷi."
}