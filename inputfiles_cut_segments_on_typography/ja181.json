{
  "ja181:0.1_0": "Jātaka",
  "ja181:0.2_0": "Dukanipāta",
  "ja181:0.3_0": "Asadisavagga",
  "ja181:0.4_0": "1. Asadisajātaka",
  "ja181:1.1_0": "Dhanuggaho asadiso,",
  "ja181:1.2_0": "rājaputto mahabbalo;",
  "ja181:1.3_0": "Dūrepātī akkhaṇavedhī,",
  "ja181:1.4_0": "mahākāyappadālano.",
  "ja181:2.1_0": "Sabbāmitte raṇaṁ katvā,",
  "ja181:2.2_0": "na ca kañci viheṭhayi;",
  "ja181:2.3_0": "Bhātaraṁ sotthiṁ katvāna,",
  "ja181:2.4_0": "saṁyamaṁ ajjhupāgamīti.",
  "ja181:3.1_0": "Asadisajātakaṁ paṭhamaṁ."
}