{
  "ja268:0.1_0": "Jātaka",
  "ja268:0.2_0": "Tikanipāta",
  "ja268:0.3_0": "Padumavagga",
  "ja268:0.4_0": "8. Ārāmadūsakajātaka",
  "ja268:1.1_0": "“Yo ve sabbasametānaṁ,",
  "ja268:1.2_0": "ahuvā seṭṭhasammato;",
  "ja268:1.3_0": "Tassāyaṁ edisī paññā,",
  "ja268:1.4_0": "kimeva itarā pajā”.",
  "ja268:2.1_0": "“Evameva tuvaṁ brahme,",
  "ja268:2.2_0": "anaññāya vinindasi;",
  "ja268:2.3_0": "Kathaṁ mūlaṁ adisvāna,",
  "ja268:2.4_0": "rukkhaṁ jaññā patiṭṭhitaṁ”.",
  "ja268:3.1_0": "“Nāhaṁ tumhe vinindāmi,",
  "ja268:3.2_0": "ye caññe vānarā vane;",
  "ja268:3.3_0": "Vissasenova gārayho,",
  "ja268:3.4_0": "yassatthā rukkharopakā”ti.",
  "ja268:4.1_0": "Ārāmadūsakajātakaṁ aṭṭhamaṁ."
}