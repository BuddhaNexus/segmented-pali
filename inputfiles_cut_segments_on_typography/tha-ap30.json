{
  "tha-ap30:0.1_0": "Therāpadāna",
  "tha-ap30:0.2_0": "Subhūtivagga",
  "tha-ap30:0.3_0": "8 Uttiyattheraapadāna",
  "tha-ap30:1.1_0": "“Candabhāgānadītīre,",
  "tha-ap30:1.2_0": "susumāro ahaṁ tadā;",
  "tha-ap30:1.3_0": "Sagocarappasutohaṁ,",
  "tha-ap30:1.4_0": "nadītitthaṁ agacchahaṁ.",
  "tha-ap30:2.1_0": "Siddhattho tamhi samaye,",
  "tha-ap30:2.2_0": "sayambhū aggapuggalo;",
  "tha-ap30:2.3_0": "Nadiṁ taritukāmo so,",
  "tha-ap30:2.4_0": "nadītitthaṁ upāgami.",
  "tha-ap30:3.1_0": "Upāgate ca sambuddhe,",
  "tha-ap30:3.2_0": "ahampi tatthupāgamiṁ;",
  "tha-ap30:3.3_0": "Upagantvāna sambuddhaṁ,",
  "tha-ap30:3.4_0": "imaṁ vācaṁ udīrayiṁ.",
  "tha-ap30:4.1_0": "‘Abhirūha mahāvīra,",
  "tha-ap30:4.2_0": "tāressāmi ahaṁ tuvaṁ;",
  "tha-ap30:4.3_0": "Pettikaṁ visayaṁ mayhaṁ,",
  "tha-ap30:4.4_0": "anukampa mahāmuni’.",
  "tha-ap30:5.1_0": "Mama uggajjanaṁ sutvā,",
  "tha-ap30:5.2_0": "abhirūhi mahāmuni;",
  "tha-ap30:5.3_0": "Haṭṭho haṭṭhena cittena,",
  "tha-ap30:5.4_0": "tāresiṁ lokanāyakaṁ.",
  "tha-ap30:6.1_0": "Nadiyā pārime tīre,",
  "tha-ap30:6.2_0": "siddhattho lokanāyako;",
  "tha-ap30:6.3_0": "Assāsesi mamaṁ tattha,",
  "tha-ap30:6.4_0": "amataṁ pāpuṇissasi.",
  "tha-ap30:7.1_0": "Tamhā kāyā cavitvāna,",
  "tha-ap30:7.2_0": "devalokaṁ āgacchahaṁ;",
  "tha-ap30:7.3_0": "Dibbasukhaṁ anubhaviṁ,",
  "tha-ap30:7.4_0": "accharāhi purakkhato.",
  "tha-ap30:8.1_0": "Sattakkhattuñca devindo,",
  "tha-ap30:8.2_0": "devarajjamakāsahaṁ;",
  "tha-ap30:8.3_0": "Tīṇikkhattuṁ cakkavattī,",
  "tha-ap30:8.4_0": "mahiyā issaro ahuṁ.",
  "tha-ap30:9.1_0": "Vivekamanuyuttohaṁ,",
  "tha-ap30:9.2_0": "nipako ca susaṁvuto;",
  "tha-ap30:9.3_0": "Dhāremi antimaṁ dehaṁ,",
  "tha-ap30:9.4_0": "sammāsambuddhasāsane.",
  "tha-ap30:10.1_0": "Catunnavutito kappe,",
  "tha-ap30:10.2_0": "tāresiṁ yaṁ narāsabhaṁ;",
  "tha-ap30:10.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap30:10.4_0": "taraṇāya idaṁ phalaṁ.",
  "tha-ap30:11.1_0": "Paṭisambhidā catasso,",
  "tha-ap30:11.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap30:11.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap30:11.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap30:12.1_0": "Itthaṁ sudaṁ āyasmā uttiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap30:13.1_0": "Uttiyattherassāpadānaṁ aṭṭhamaṁ."
}