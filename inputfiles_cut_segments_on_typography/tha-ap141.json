{
  "tha-ap141:0.1_0": "Therāpadāna",
  "tha-ap141:0.2_0": "Sobhitavagga",
  "tha-ap141:0.3_0": "9 Sālapupphadāyakattheraapadāna",
  "tha-ap141:1.1_0": "“Migarājā tadā āsiṁ,",
  "tha-ap141:1.2_0": "abhijāto sukesarī;",
  "tha-ap141:1.3_0": "Giriduggaṁ gavesanto,",
  "tha-ap141:1.4_0": "addasaṁ lokanāyakaṁ.",
  "tha-ap141:2.1_0": "Ayaṁ nu kho mahāvīro,",
  "tha-ap141:2.2_0": "nibbāpeti mahājanaṁ;",
  "tha-ap141:2.3_0": "Yannūnāhaṁ upāseyyaṁ,",
  "tha-ap141:2.4_0": "devadevaṁ narāsabhaṁ.",
  "tha-ap141:3.1_0": "Sākhaṁ sālassa bhañjitvā,",
  "tha-ap141:3.2_0": "sakosaṁ pupphamāhariṁ;",
  "tha-ap141:3.3_0": "Upagantvāna sambuddhaṁ,",
  "tha-ap141:3.4_0": "adāsiṁ pupphamuttamaṁ.",
  "tha-ap141:4.1_0": "Ekanavutito kappe,",
  "tha-ap141:4.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap141:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap141:4.4_0": "pupphadānassidaṁ phalaṁ.",
  "tha-ap141:5.1_0": "Ito ca navame kappe,",
  "tha-ap141:5.2_0": "virocanasanāmakā;",
  "tha-ap141:5.3_0": "Tayo āsiṁsu rājāno,",
  "tha-ap141:5.4_0": "cakkavattī mahabbalā.",
  "tha-ap141:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap141:6.2_0": "…pe…",
  "tha-ap141:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap141:7.1_0": "Itthaṁ sudaṁ āyasmā sālapupphadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap141:8.1_0": "Sālapupphadāyakattherassāpadānaṁ navamaṁ."
}