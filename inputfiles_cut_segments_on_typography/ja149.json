{
  "ja149:0.1_0": "Jātaka",
  "ja149:0.2_0": "Ekakanipāta",
  "ja149:0.3_0": "Kakaṇṭakavagga",
  "ja149:0.4_0": "9. Ekapaṇṇajātaka",
  "ja149:1.1_0": "“Ekapaṇṇo ayaṁ rukkho,",
  "ja149:1.2_0": "na bhūmyā caturaṅgulo;",
  "ja149:1.3_0": "Phalena visakappena,",
  "ja149:1.4_0": "mahāyaṁ kiṁ bhavissatī”ti.",
  "ja149:2.1_0": "Ekapaṇṇajātakaṁ navamaṁ."
}