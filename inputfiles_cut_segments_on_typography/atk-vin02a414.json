{
  "atk-vin02a414:0_0": "Āpattisamuṭṭhānavaṇṇanā",
  "atk-vin02a414:1_0": "470.Acittakoāpajjatītiādīsu sahaseyyādipaṇṇattivajjaṁ asañcicca āpajjanto acittako āpajjati,",
  "atk-vin02a414:1_1": "desento sacittako vuṭṭhāti.",
  "atk-vin02a414:1_2": "Yaṁkiñci sañcicca āpajjanto sacittako āpajjati,",
  "atk-vin02a414:1_3": "tiṇavatthārakena vuṭṭhahanto acittako vuṭṭhāti.",
  "atk-vin02a414:1_4": "Pubbe vuttameva tiṇavatthārakena vuṭṭhahanto acittako āpajjati,",
  "atk-vin02a414:1_5": "acittako vuṭṭhāti.",
  "atk-vin02a414:1_6": "Itaraṁ desento sacittako āpajjati,",
  "atk-vin02a414:1_7": "sacittako vuṭṭhāti.",
  "atk-vin02a414:1_8": "“Dhammadānaṁ karomī”ti padasodhammādīni karonto kusalacitto āpajjati,",
  "atk-vin02a414:1_9": "“buddhānaṁ anusāsaniṁ karomī”ti udaggacitto desento kusalacitto vuṭṭhāti.",
  "atk-vin02a414:1_10": "Domanassiko hutvā desento akusalacitto vuṭṭhāti,",
  "atk-vin02a414:1_11": "tiṇavatthārakena niddāgatova vuṭṭhahanto abyākatacitto vuṭṭhāti.",
  "atk-vin02a414:1_12": "Bhiṁsāpanādīni katvā “buddhānaṁ sāsanaṁ karomī”ti somanassiko desento akusalacitto āpajjati,",
  "atk-vin02a414:1_13": "kusalacitto vuṭṭhāti.",
  "atk-vin02a414:1_14": "Domanassikova desento akusalacitto vuṭṭhāti,",
  "atk-vin02a414:1_15": "vuttanayeneva tiṇavatthārakena vuṭṭhahanto abyākatacitto vuṭṭhāti.",
  "atk-vin02a414:1_16": "Niddokkantasamaye sahagāraseyyaṁ āpajjanto abyākatacitto āpajjati,",
  "atk-vin02a414:1_17": "vuttanayeneva panettha “kusalacitto vuṭṭhātī”tiādi veditabbaṁ.",
  "atk-vin02a414:2_0": "Paṭhamaṁ pārājikaṁ katihi samuṭṭhānehītiādi pubbe vuttanayattā uttānameva.",
  "atk-vin02a414:3_0": "473.Cattāro pārājikā katihi samuṭṭhānehītiādīsu ukkaṭṭhaparicchedato yaṁ yaṁ samuṭṭhānaṁ yassa yassa labbhati,",
  "atk-vin02a414:3_1": "taṁ sabbaṁ vuttameva hoti.",
  "atk-vin02a414:4_0": "Āpattisamuṭṭhānavaṇṇanā niṭṭhitā."
}