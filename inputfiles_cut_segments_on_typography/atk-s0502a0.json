{
  "atk-s0502a0:0_0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "atk-s0502a0:1_0": "Khuddakanikāye",
  "atk-s0502a0:2_0": "Dhammapada-aṭṭhakathā",
  "atk-s0502a0:3_0": "(Paṭhamo bhāgo)",
  "atk-s0502a0:4_0": "Ganthārambhakathā",
  "atk-s0502a0:5_0": "1.",
  "atk-s0502a0:6_0": "Mahāmohatamonaddhe, loke lokantadassinā;",
  "atk-s0502a0:7_0": "Yena saddhammapajjoto, jālito jalitiddhinā.",
  "atk-s0502a0:8_0": "2.",
  "atk-s0502a0:9_0": "Tassa pāde namassitvā, sambuddhassa sirīmato;",
  "atk-s0502a0:10_0": "Saddhammañcassa pūjetvā, katvā saṅghassa cañjaliṁ.",
  "atk-s0502a0:11_0": "3.",
  "atk-s0502a0:12_0": "Taṁ taṁ kāraṇamāgamma, dhammādhammesu kovido;",
  "atk-s0502a0:13_0": "Sampattasaddhammapado, satthā dhammapadaṁ subhaṁ.",
  "atk-s0502a0:14_0": "4.",
  "atk-s0502a0:15_0": "Desesi karuṇāvega-samussāhitamānaso;",
  "atk-s0502a0:16_0": "Yaṁ ve devamanussānaṁ, pītipāmojjavaḍḍhanaṁ.",
  "atk-s0502a0:17_0": "5.",
  "atk-s0502a0:18_0": "Paramparābhatā tassa, nipuṇā atthavaṇṇanā;",
  "atk-s0502a0:19_0": "Yā tambapaṇṇidīpamhi, dīpabhāsāya saṇṭhitā.",
  "atk-s0502a0:20_0": "6.",
  "atk-s0502a0:21_0": "Na sādhayati sesānaṁ, sattānaṁ hitasampadaṁ;",
  "atk-s0502a0:22_0": "Appeva nāma sādheyya, sabbalokassa sā hitaṁ.",
  "atk-s0502a0:23_0": "7.",
  "atk-s0502a0:24_0": "Iti āsīsamānena, dantena samacārinā;",
  "atk-s0502a0:25_0": "Kumārakassapenāhaṁ, therena thiracetasā.",
  "atk-s0502a0:26_0": "8.",
  "atk-s0502a0:27_0": "Saddhammaṭṭhitikāmena, sakkaccaṁ abhiyācito;",
  "atk-s0502a0:28_0": "Taṁ bhāsaṁ ativitthāra-gatañca vacanakkamaṁ.",
  "atk-s0502a0:29_0": "9.",
  "atk-s0502a0:30_0": "Pahāyāropayitvāna, tantibhāsaṁ manoramaṁ;",
  "atk-s0502a0:31_0": "Gāthānaṁ byañjanapadaṁ, yaṁ tattha na vibhāvitaṁ.",
  "atk-s0502a0:32_0": "10.",
  "atk-s0502a0:33_0": "Kevalaṁ taṁ vibhāvetvā, sesaṁ tameva atthato;",
  "atk-s0502a0:34_0": "Bhāsantarena bhāsissaṁ, āvahanto vibhāvinaṁ;",
  "atk-s0502a0:35_0": "Manaso pītipāmojjaṁ, atthadhammūpanissitanti."
}