{
  "tha-ap297:0.1_0": "Therāpadāna",
  "tha-ap297:0.2_0": "Citakapūjakavagga",
  "tha-ap297:0.3_0": "5. Gosīsanikkhepakattheraapadāna",
  "tha-ap297:1.1_0": "“Ārāmadvārā nikkhamma,",
  "tha-ap297:1.2_0": "gosīsaṁ santhataṁ mayā;",
  "tha-ap297:1.3_0": "Anubhomi sakaṁ kammaṁ,",
  "tha-ap297:1.4_0": "pubbakammassidaṁ phalaṁ.",
  "tha-ap297:2.1_0": "Ājāniyā vātajavā,",
  "tha-ap297:2.2_0": "sindhavā sīghavāhanā;",
  "tha-ap297:2.3_0": "Anubhomi sabbametaṁ,",
  "tha-ap297:2.4_0": "gosīsassa idaṁ phalaṁ.",
  "tha-ap297:3.1_0": "Aho kāraṁ paramakāraṁ,",
  "tha-ap297:3.2_0": "sukhette sukataṁ mayā;",
  "tha-ap297:3.3_0": "Saṅghe katassa kārassa,",
  "tha-ap297:3.4_0": "na aññaṁ kalamagghati.",
  "tha-ap297:4.1_0": "Catunnavutito kappe,",
  "tha-ap297:4.2_0": "yaṁ sīsaṁ santhariṁ ahaṁ;",
  "tha-ap297:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap297:4.4_0": "santharassa idaṁ phalaṁ.",
  "tha-ap297:5.1_0": "Pañcasattatikappamhi,",
  "tha-ap297:5.2_0": "suppatiṭṭhitanāmako;",
  "tha-ap297:5.3_0": "Eko āsiṁ mahātejo,",
  "tha-ap297:5.4_0": "cakkavattī mahabbalo.",
  "tha-ap297:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap297:6.2_0": "…pe…",
  "tha-ap297:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap297:7.1_0": "Itthaṁ sudaṁ āyasmā gosīsanikkhepako thero imā gāthāyo abhāsitthāti.",
  "tha-ap297:8.1_0": "Gosīsanikkhepakattherassāpadānaṁ pañcamaṁ."
}