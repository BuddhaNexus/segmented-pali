{
  "thag1.75:0.1_0": "Theragāthā 1.75",
  "thag1.75:0.2_0": "Ekakanipāta",
  "thag1.75:0.3_0": "Aṭṭhamavagga",
  "thag1.75:0.4_0": "Susāradattheragāthā",
  "thag1.75:1.1_0": "“Sādhu suvihitāna dassanaṁ,",
  "thag1.75:1.2_0": "Kaṅkhā chijjati buddhi vaḍḍhati;",
  "thag1.75:1.3_0": "Bālampi karonti paṇḍitaṁ,",
  "thag1.75:1.4_0": "Tasmā sādhu sataṁ samāgamo”ti.",
  "thag1.75:1.5_0": "… Susārado thero …."
}