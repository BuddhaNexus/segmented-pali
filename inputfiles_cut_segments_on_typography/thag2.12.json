{
  "thag2.12:0.1_0": "Theragāthā 2.12",
  "thag2.12:0.2_0": "Dukanipāta",
  "thag2.12:0.3_0": "Dutiyavagga",
  "thag2.12:0.4_0": "Jotidāsattheragāthā",
  "thag2.12:1.1_0": "“Ye kho te veṭhamissena,",
  "thag2.12:1.2_0": "nānattena ca kammunā;",
  "thag2.12:1.3_0": "Manusse uparundhanti,",
  "thag2.12:1.4_0": "pharusūpakkamā janā;",
  "thag2.12:1.5_0": "Tepi tattheva kīranti,",
  "thag2.12:1.6_0": "na hi kammaṁ panassati.",
  "thag2.12:2.1_0": "Yaṁ karoti naro kammaṁ,",
  "thag2.12:2.2_0": "Kalyāṇaṁ yadi pāpakaṁ;",
  "thag2.12:2.3_0": "Tassa tasseva dāyādo,",
  "thag2.12:2.4_0": "Yaṁ yaṁ kammaṁ pakubbatī”ti.",
  "thag2.12:2.5_0": "… Jotidāso thero …."
}