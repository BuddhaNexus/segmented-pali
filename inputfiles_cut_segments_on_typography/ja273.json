{
  "ja273:0.1_0": "Jātaka",
  "ja273:0.2_0": "Tikanipāta",
  "ja273:0.3_0": "Udapānavagga",
  "ja273:0.4_0": "3. Kacchapajātaka",
  "ja273:1.1_0": "“Ko nu uddhitabhattova,",
  "ja273:1.2_0": "pūrahatthova brāhmaṇo;",
  "ja273:1.3_0": "Kahaṁ nu bhikkhaṁ acari,",
  "ja273:1.4_0": "kaṁ saddhaṁ upasaṅkami”.",
  "ja273:2.1_0": "“Ahaṁ kapismi dummedho,",
  "ja273:2.2_0": "anāmāsāni āmasiṁ;",
  "ja273:2.3_0": "Tvaṁ maṁ mocaya bhaddaṁ te,",
  "ja273:2.4_0": "mutto gaccheyya pabbataṁ”.",
  "ja273:3.1_0": "“Kacchapā kassapā honti,",
  "ja273:3.2_0": "Koṇḍaññā honti makkaṭā;",
  "ja273:3.3_0": "Muñca kassapa koṇḍaññaṁ,",
  "ja273:3.4_0": "Kataṁ methunakaṁ tayā”ti.",
  "ja273:4.1_0": "Kacchapajātakaṁ tatiyaṁ."
}