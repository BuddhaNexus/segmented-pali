{
  "thi-ap10:0.1_0": "Therīapadāna",
  "thi-ap10:0.2_0": "Sumedhāvagga",
  "thi-ap10:0.3_0": "10. Udakadāyikātherīapadāna",
  "thi-ap10:1.1_0": "“Nagare bandhumatiyā,",
  "thi-ap10:1.2_0": "ahosiṁ udahārikā;",
  "thi-ap10:1.3_0": "Udahārena jīvāmi,",
  "thi-ap10:1.4_0": "tena posemi dārake.",
  "thi-ap10:2.1_0": "Deyyadhammo ca me natthi,",
  "thi-ap10:2.2_0": "puññakkhette anuttare;",
  "thi-ap10:2.3_0": "Koṭṭhakaṁ upasaṅkamma,",
  "thi-ap10:2.4_0": "udakaṁ paṭṭhapesahaṁ.",
  "thi-ap10:3.1_0": "Tena kammena sukatena,",
  "thi-ap10:3.2_0": "tāvatiṁsamagacchahaṁ;",
  "thi-ap10:3.3_0": "Tattha me sukataṁ byamhaṁ,",
  "thi-ap10:3.4_0": "udahārena nimmitaṁ.",
  "thi-ap10:4.1_0": "Accharānaṁ sahassassa,",
  "thi-ap10:4.2_0": "ahañhi pavarā tadā;",
  "thi-ap10:4.3_0": "Dasaṭṭhānehi tā sabbā,",
  "thi-ap10:4.4_0": "abhibhomi sadā ahaṁ.",
  "thi-ap10:5.1_0": "Paññāsaṁ devarājūnaṁ,",
  "thi-ap10:5.2_0": "mahesittamakārayiṁ;",
  "thi-ap10:5.3_0": "Vīsaticakkavattīnaṁ,",
  "thi-ap10:5.4_0": "mahesittamakārayiṁ.",
  "thi-ap10:6.1_0": "Duve bhave saṁsarāmi,",
  "thi-ap10:6.2_0": "devatte atha mānuse;",
  "thi-ap10:6.3_0": "Duggatiṁ nābhijānāmi,",
  "thi-ap10:6.4_0": "dakadānassidaṁ phalaṁ.",
  "thi-ap10:7.1_0": "Pabbatagge dumagge vā,",
  "thi-ap10:7.2_0": "antalikkhe ca bhūmiyaṁ;",
  "thi-ap10:7.3_0": "Yadā udakamicchāmi,",
  "thi-ap10:7.4_0": "khippaṁ paṭilabhāmahaṁ.",
  "thi-ap10:8.1_0": "Avuṭṭhikā disā natthi,",
  "thi-ap10:8.2_0": "santattā kuthitāpi ca;",
  "thi-ap10:8.3_0": "Mama saṅkappamaññāya,",
  "thi-ap10:8.4_0": "mahāmegho pavassati.",
  "thi-ap10:9.1_0": "Kadāci nīyamānāya,",
  "thi-ap10:9.2_0": "ñātisaṅghena me tadā;",
  "thi-ap10:9.3_0": "Yadā icchāmahaṁ vassaṁ,",
  "thi-ap10:9.4_0": "mahāmegho ajāyatha.",
  "thi-ap10:10.1_0": "Uṇhaṁ vā pariḷāho vā,",
  "thi-ap10:10.2_0": "sarīre me na vijjati;",
  "thi-ap10:10.3_0": "Kāye ca me rajo natthi,",
  "thi-ap10:10.4_0": "dakadānassidaṁ phalaṁ.",
  "thi-ap10:11.1_0": "Visuddhamanasā ajja,",
  "thi-ap10:11.2_0": "apetamanapāpikā;",
  "thi-ap10:11.3_0": "Sabbāsavaparikkhīṇā,",
  "thi-ap10:11.4_0": "natthi dāni punabbhavo.",
  "thi-ap10:12.1_0": "Ekanavutito kappe,",
  "thi-ap10:12.2_0": "yaṁ dakaṁ adadiṁ tadā;",
  "thi-ap10:12.3_0": "Duggatiṁ nābhijānāmi,",
  "thi-ap10:12.4_0": "dakadānassidaṁ phalaṁ.",
  "thi-ap10:13.1_0": "Kilesā jhāpitā mayhaṁ,",
  "thi-ap10:13.2_0": "…pe…",
  "thi-ap10:13.3_0": "viharāmi anāsavā.",
  "thi-ap10:14.1_0": "Svāgataṁ vata me āsi,",
  "thi-ap10:14.2_0": "…pe…",
  "thi-ap10:14.3_0": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap10:15.1_0": "Paṭisambhidā catasso,",
  "thi-ap10:15.2_0": "…pe…",
  "thi-ap10:15.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "thi-ap10:16.1_0": "Itthaṁ sudaṁ udakadāyikā bhikkhunī imā gāthāyo abhāsitthāti.",
  "thi-ap10:17.1_0": "Udakadāyikātheriyāpadānaṁ dasamaṁ.",
  "thi-ap10:18.1_0": "Sumedhāvaggo paṭhamo.",
  "thi-ap10:19.1_0": "Tassuddānaṁ",
  "thi-ap10:20.1_0": "Sumedhā mekhalādāyī,",
  "thi-ap10:20.2_0": "maṇḍapaṁ saṅkamaṁ dadā;",
  "thi-ap10:20.3_0": "Naḷamālī piṇḍadadā,",
  "thi-ap10:20.4_0": "kaṭacchu uppalappadā.",
  "thi-ap10:21.1_0": "Dīpadā dakadā ceva,",
  "thi-ap10:21.2_0": "gāthāyo gaṇitā iha;",
  "thi-ap10:21.3_0": "Ekagāthāsatañceva,",
  "thi-ap10:21.4_0": "tiṁsati ca taduttari."
}