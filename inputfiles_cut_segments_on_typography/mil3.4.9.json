{
  "mil3.4.9:0.1_0": "Milindapañha",
  "mil3.4.9:0.2_0": "Nibbānavagga",
  "mil3.4.9:0.3_0": "9. Nibbānalabhanapañha",
  "mil3.4.9:1.1_0": "Rājā āha—",
  "mil3.4.9:1.2_0": "“bhante nāgasena, sabbeva labhanti nibbānan”ti?",
  "mil3.4.9:1.3_0": "“Na kho, mahārāja,",
  "mil3.4.9:1.3_1": "sabbeva labhanti nibbānaṁ,",
  "mil3.4.9:1.3_2": "api ca kho, mahārāja,",
  "mil3.4.9:1.3_3": "yo sammā paṭipanno abhiññeyye dhamme abhijānāti,",
  "mil3.4.9:1.3_4": "pariññeyye dhamme parijānāti,",
  "mil3.4.9:1.3_5": "pahātabbe dhamme pajahati,",
  "mil3.4.9:1.3_6": "bhāvetabbe dhamme bhāveti,",
  "mil3.4.9:1.3_7": "sacchikātabbe dhamme sacchikaroti,",
  "mil3.4.9:1.3_8": "so labhati nibbānan”ti.",
  "mil3.4.9:2.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.4.9:3.1_0": "Nibbānalabhanapañho navamo."
}