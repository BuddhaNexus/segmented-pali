{
  "ja113:0.1_0": "Jātaka",
  "ja113:0.2_0": "Ekakanipāta",
  "ja113:0.3_0": "Haṁcivagga",
  "ja113:0.4_0": "3. Siṅgālajātaka",
  "ja113:1.1_0": "“Saddahāsi siṅgālassa,",
  "ja113:1.2_0": "surāpītassa brāhmaṇa;",
  "ja113:1.3_0": "Sippikānaṁ sataṁ natthi,",
  "ja113:1.4_0": "kuto kaṁsasatā duve”ti.",
  "ja113:2.1_0": "Siṅgālajātakaṁ tatiyaṁ."
}