{
  "thag1.115:0.1_0": "Theragāthā 1.115",
  "thag1.115:0.2_0": "Ekakanipāta",
  "thag1.115:0.3_0": "Dvādasamavagga",
  "thag1.115:0.4_0": "Mahānāmattheragāthā",
  "thag1.115:1.1_0": "“Esāvahiyyase pabbatena,",
  "thag1.115:1.2_0": "Bahukuṭajasallakikena;",
  "thag1.115:1.3_0": "Nesādakena girinā,",
  "thag1.115:1.4_0": "Yasassinā paricchadenā”ti.",
  "thag1.115:1.5_0": "… Mahānāmo thero …."
}