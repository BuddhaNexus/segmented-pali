{
  "tika-s0402t16:0_0": "2. Akusalapeyyālaṁ",
  "tika-s0402t16:1_0": "191- 200.",
  "tika-s0402t16:1_1": "Dukkhassa vaḍḍhi etesanti dukkhavaḍḍhikā.",
  "tika-s0402t16:1_2": "Ye hi dukkhaṁ vaḍḍhenti,",
  "tika-s0402t16:1_3": "punappunaṁ uppādenti,",
  "tika-s0402t16:1_4": "dukkhassa vaḍḍhi tesaṁ atthīti evaṁ vuttaṁ.",
  "tika-s0402t16:1_5": "Sukhavaḍḍhikāti etthāpi eseva nayo."
}