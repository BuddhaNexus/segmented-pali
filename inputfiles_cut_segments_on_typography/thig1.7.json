{
  "thig1.7:0.1_0": "Therīgāthā 1.7",
  "thig1.7:0.2_0": "Ekakanipāta",
  "thig1.7:0.3_0": "Vīrātherīgāthā",
  "thig1.7:1.1_0": "“Vīrā vīrehi dhammehi,",
  "thig1.7:1.2_0": "bhikkhunī bhāvitindriyā;",
  "thig1.7:1.3_0": "Dhāreti antimaṁ dehaṁ,",
  "thig1.7:1.4_0": "jetvā māraṁ savāhinin”ti.",
  "thig1.7:2.1_0": "… Vīrā therī …."
}