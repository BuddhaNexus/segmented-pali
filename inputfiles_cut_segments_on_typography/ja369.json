{
  "ja369:0.1_0": "Jātaka",
  "ja369:0.2_0": "Pañcakanipāta",
  "ja369:0.3_0": "Vaṇṇārohavagga",
  "ja369:0.4_0": "9. Mittavindakajātaka",
  "ja369:1.1_0": "“Kyāhaṁ devānamakaraṁ,",
  "ja369:1.2_0": "kiṁ pāpaṁ pakataṁ mayā;",
  "ja369:1.3_0": "Yaṁ me sirasmiṁ ohacca,",
  "ja369:1.4_0": "cakkaṁ bhamati matthake”.",
  "ja369:2.1_0": "“Atikkamma ramaṇakaṁ,",
  "ja369:2.2_0": "sadāmattañca dūbhakaṁ;",
  "ja369:2.3_0": "Brahmattarañca pāsādaṁ,",
  "ja369:2.4_0": "kenatthena idhāgato”.",
  "ja369:3.1_0": "“Ito bahutarā bhogā,",
  "ja369:3.2_0": "atra maññe bhavissare;",
  "ja369:3.3_0": "Iti etāya saññāya,",
  "ja369:3.4_0": "passa maṁ byasanaṁ gataṁ”.",
  "ja369:4.1_0": "“Catubbhi aṭṭhajjhagamā,",
  "ja369:4.2_0": "aṭṭhāhipi ca soḷasa;",
  "ja369:4.3_0": "Soḷasāhi ca bāttiṁsa,",
  "ja369:4.4_0": "atricchaṁ cakkamāsado;",
  "ja369:4.5_0": "Icchāhatassa posassa,",
  "ja369:4.6_0": "cakkaṁ bhamati matthake.",
  "ja369:5.1_0": "Uparivisālā duppūrā,",
  "ja369:5.2_0": "icchā visaṭagāminī;",
  "ja369:5.3_0": "Ye ca taṁ anugijjhanti,",
  "ja369:5.4_0": "te honti cakkadhārino”ti.",
  "ja369:6.1_0": "Mittavindakajātakaṁ navamaṁ."
}