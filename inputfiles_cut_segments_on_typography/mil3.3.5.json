{
  "mil3.3.5:0.1_0": "Milindapañha",
  "mil3.3.5:0.2_0": "Vicāravagga",
  "mil3.3.5:0.3_0": "5. Bhavantasaṅkhārajāyamānapañha",
  "mil3.3.5:1.1_0": "Rājā āha—",
  "mil3.3.5:1.2_0": "“bhante nāgasena, atthi keci saṅkhārā, ye abhavantā jāyantī”ti?",
  "mil3.3.5:1.3_0": "“Natthi, mahārāja,",
  "mil3.3.5:1.3_1": "keci saṅkhārā,",
  "mil3.3.5:1.3_2": "ye abhavantā jāyanti,",
  "mil3.3.5:1.3_3": "bhavantāyeva kho, mahārāja,",
  "mil3.3.5:1.3_4": "saṅkhārā jāyantī”ti.",
  "mil3.3.5:2.1_0": "“Opammaṁ karohī”ti.",
  "mil3.3.5:2.2_0": "“Taṁ kiṁ maññasi, mahārāja, idaṁ gehaṁ abhavantaṁ jātaṁ, yattha tvaṁ nisinnosī”ti?",
  "mil3.3.5:2.3_0": "“Natthi kiñci, bhante,",
  "mil3.3.5:2.3_1": "idha abhavantaṁ jātaṁ,",
  "mil3.3.5:2.3_2": "bhavantaṁyeva jātaṁ,",
  "mil3.3.5:2.3_3": "imāni kho, bhante,",
  "mil3.3.5:2.3_4": "dārūni vane ahesuṁ,",
  "mil3.3.5:2.3_5": "ayañca mattikā pathaviyaṁ ahosi,",
  "mil3.3.5:2.3_6": "itthīnañca purisānañca tajjena vāyāmena evamidaṁ gehaṁ nibbattan”ti.",
  "mil3.3.5:2.4_0": "“Evameva kho, mahārāja,",
  "mil3.3.5:2.4_1": "natthi keci saṅkhārā,",
  "mil3.3.5:2.4_2": "ye abhavantā jāyanti,",
  "mil3.3.5:2.4_3": "bhavantāyeva saṅkhārā jāyantī”ti.",
  "mil3.3.5:3.1_0": "“Bhiyyo opammaṁ karohī”ti.",
  "mil3.3.5:3.2_0": "“Yathā, mahārāja,",
  "mil3.3.5:3.2_1": "ye keci bījagāmabhūtagāmā pathaviyaṁ nikkhittā anupubbena vuḍḍhiṁ virūḷhiṁ vepullaṁ āpajjamānā pupphāni ca phalāni ca dadeyyuṁ,",
  "mil3.3.5:3.2_2": "na te rukkhā abhavantā jātā,",
  "mil3.3.5:3.2_3": "bhavantāyeva te rukkhā jātā;",
  "mil3.3.5:3.3_0": "evameva kho, mahārāja,",
  "mil3.3.5:3.3_1": "natthi keci saṅkhārā,",
  "mil3.3.5:3.3_2": "ye abhavantā jāyanti,",
  "mil3.3.5:3.3_3": "bhavantāyeva te saṅkhārā jāyantī”ti.",
  "mil3.3.5:4.1_0": "“Bhiyyo opammaṁ karohī”ti.",
  "mil3.3.5:4.2_0": "“Yathā, mahārāja,",
  "mil3.3.5:4.2_1": "kumbhakāro pathaviyā mattikaṁ uddharitvā nānābhājanāni karoti,",
  "mil3.3.5:4.2_2": "na tāni bhājanāni abhavantāni jātāni,",
  "mil3.3.5:4.2_3": "bhavantāniyeva jātāni;",
  "mil3.3.5:4.3_0": "evameva kho, mahārāja, natthi keci saṅkhārā, ye abhavantā jāyanti, bhavantāyeva saṅkhārā jāyantī”ti.",
  "mil3.3.5:5.1_0": "“Bhiyyo opammaṁ karohī”ti.",
  "mil3.3.5:5.2_0": "“Yathā, mahārāja,",
  "mil3.3.5:5.2_1": "vīṇāya pattaṁ na siyā,",
  "mil3.3.5:5.2_2": "cammaṁ na siyā,",
  "mil3.3.5:5.2_3": "doṇi na siyā,",
  "mil3.3.5:5.2_4": "daṇḍo na siyā,",
  "mil3.3.5:5.2_5": "upavīṇo na siyā,",
  "mil3.3.5:5.2_6": "tantiyo na siyuṁ,",
  "mil3.3.5:5.2_7": "koṇo na siyā,",
  "mil3.3.5:5.2_8": "purisassa ca tajjo vāyāmo na siyā,",
  "mil3.3.5:5.2_9": "jāyeyya saddo”ti?",
  "mil3.3.5:5.3_0": "“Na hi, bhante”ti.",
  "mil3.3.5:5.4_0": "“Yato ca kho, mahārāja,",
  "mil3.3.5:5.4_1": "vīṇāya pattaṁ siyā,",
  "mil3.3.5:5.4_2": "cammaṁ siyā,",
  "mil3.3.5:5.4_3": "doṇi siyā,",
  "mil3.3.5:5.4_4": "daṇḍo siyā,",
  "mil3.3.5:5.4_5": "upavīṇo siyā,",
  "mil3.3.5:5.4_6": "tantiyo siyuṁ,",
  "mil3.3.5:5.4_7": "koṇo siyā,",
  "mil3.3.5:5.4_8": "purisassa ca tajjo vāyāmo siyā,",
  "mil3.3.5:5.4_9": "jāyeyya saddo”ti?",
  "mil3.3.5:5.5_0": "“Āma, bhante, jāyeyyā”ti.",
  "mil3.3.5:5.6_0": "“Evameva kho, mahārāja,",
  "mil3.3.5:5.6_1": "natthi keci saṅkhārā,",
  "mil3.3.5:5.6_2": "ye abhavantā jāyanti,",
  "mil3.3.5:5.6_3": "bhavantāyeva kho saṅkhārā jāyantī”ti.",
  "mil3.3.5:6.1_0": "“Bhiyyo opammaṁ karohī”ti.",
  "mil3.3.5:6.2_0": "“Yathā, mahārāja,",
  "mil3.3.5:6.2_1": "araṇi na siyā,",
  "mil3.3.5:6.2_2": "araṇipotako na siyā,",
  "mil3.3.5:6.2_3": "araṇiyottakaṁ na siyā,",
  "mil3.3.5:6.2_4": "uttarāraṇi na siyā,",
  "mil3.3.5:6.2_5": "coḷakaṁ na siyā,",
  "mil3.3.5:6.2_6": "purisassa ca tajjo vāyāmo na siyā,",
  "mil3.3.5:6.2_7": "jāyeyya so aggī”ti?",
  "mil3.3.5:6.3_0": "“Na hi, bhante”ti.",
  "mil3.3.5:6.4_0": "“Yato ca kho, mahārāja,",
  "mil3.3.5:6.4_1": "araṇi siyā,",
  "mil3.3.5:6.4_2": "araṇipotako siyā,",
  "mil3.3.5:6.4_3": "araṇiyottakaṁ siyā,",
  "mil3.3.5:6.4_4": "uttarāraṇi siyā,",
  "mil3.3.5:6.4_5": "coḷakaṁ siyā,",
  "mil3.3.5:6.4_6": "purisassa ca tajjo vāyāmo siyā,",
  "mil3.3.5:6.4_7": "jāyeyya so aggī”ti?",
  "mil3.3.5:6.5_0": "“Āma, bhante, jāyeyyā”ti.",
  "mil3.3.5:6.6_0": "“Evameva kho, mahārāja,",
  "mil3.3.5:6.6_1": "natthi keci saṅkhārā,",
  "mil3.3.5:6.6_2": "ye abhavantā jāyanti,",
  "mil3.3.5:6.6_3": "bhavantāyeva kho saṅkhārā jāyantī”ti.",
  "mil3.3.5:7.1_0": "“Bhiyyo opammaṁ karohī”ti.",
  "mil3.3.5:7.2_0": "“Yathā, mahārāja, maṇi na siyā, ātapo na siyā, gomayaṁ na siyā, jāyeyya so aggī”ti?",
  "mil3.3.5:7.3_0": "“Na hi, bhante”ti.",
  "mil3.3.5:7.4_0": "“Yato ca kho, mahārāja, maṇi siyā, ātapo siyā, gomayaṁ siyā, jāyeyya so aggī”ti?",
  "mil3.3.5:7.5_0": "“Āma, bhante, jāyeyyā”ti.",
  "mil3.3.5:7.6_0": "“Evameva kho, mahārāja,",
  "mil3.3.5:7.6_1": "natthi keci saṅkhārā ye abhavantā jāyanti,",
  "mil3.3.5:7.6_2": "bhavantāyeva kho saṅkhārā jāyantī”ti.",
  "mil3.3.5:8.1_0": "“Bhiyyo opammaṁ karohī”ti.",
  "mil3.3.5:8.2_0": "“Yathā, mahārāja, ādāso na siyā, ābhā na siyā, mukhaṁ na siyā, jāyeyya attā”ti?",
  "mil3.3.5:8.3_0": "“Na hi, bhante”ti.",
  "mil3.3.5:8.4_0": "“Yato ca kho, mahārāja, ādāso siyā, ābhā siyā, mukhaṁ siyā, jāyeyya attā”ti?",
  "mil3.3.5:8.5_0": "“Āma, bhante, jāyeyyā”ti.",
  "mil3.3.5:8.6_0": "“Evameva kho, mahārāja,",
  "mil3.3.5:8.6_1": "natthi keci saṅkhārā,",
  "mil3.3.5:8.6_2": "ye abhavantā jāyanti,",
  "mil3.3.5:8.6_3": "bhavantāyeva kho saṅkhārā jāyantī”ti.",
  "mil3.3.5:9.1_0": "“Kallosi, bhante nāgasenā”ti.",
  "mil3.3.5:10.1_0": "Bhavantasaṅkhārajāyamānapañho pañcamo."
}