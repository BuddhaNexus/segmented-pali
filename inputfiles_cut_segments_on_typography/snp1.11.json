{
  "snp1.11:0.1_0": "Sutta Nipāta 1.11",
  "snp1.11:0.2_0": "Vijayasutta",
  "snp1.11:1.1_0": "Caraṁ vā yadi vā tiṭṭhaṁ,",
  "snp1.11:1.2_0": "nisinno uda vā sayaṁ;",
  "snp1.11:1.3_0": "Samiñjeti pasāreti,",
  "snp1.11:1.4_0": "esā kāyassa iñjanā.",
  "snp1.11:2.1_0": "Aṭṭhinahārusaṁyutto,",
  "snp1.11:2.2_0": "tacamaṁsāvalepano;",
  "snp1.11:2.3_0": "Chaviyā kāyo paṭicchanno,",
  "snp1.11:2.4_0": "yathābhūtaṁ na dissati.",
  "snp1.11:3.1_0": "Antapūro udarapūro,",
  "snp1.11:3.2_0": "yakanapeḷassa vatthino;",
  "snp1.11:3.3_0": "Hadayassa papphāsassa,",
  "snp1.11:3.4_0": "vakkassa pihakassa ca.",
  "snp1.11:4.1_0": "Siṅghāṇikāya kheḷassa,",
  "snp1.11:4.2_0": "sedassa ca medassa ca;",
  "snp1.11:4.3_0": "Lohitassa lasikāya,",
  "snp1.11:4.4_0": "pittassa ca vasāya ca.",
  "snp1.11:5.1_0": "Athassa navahi sotehi,",
  "snp1.11:5.2_0": "asucī savati sabbadā;",
  "snp1.11:5.3_0": "Akkhimhā akkhigūthako,",
  "snp1.11:5.4_0": "kaṇṇamhā kaṇṇagūthako.",
  "snp1.11:6.1_0": "Siṅghāṇikā ca nāsato,",
  "snp1.11:6.2_0": "mukhena vamatekadā;",
  "snp1.11:6.3_0": "Pittaṁ semhañca vamati,",
  "snp1.11:6.4_0": "kāyamhā sedajallikā.",
  "snp1.11:7.1_0": "Athassa susiraṁ sīsaṁ,",
  "snp1.11:7.2_0": "matthaluṅgassa pūritaṁ;",
  "snp1.11:7.3_0": "Subhato naṁ maññati bālo,",
  "snp1.11:7.4_0": "avijjāya purakkhato.",
  "snp1.11:8.1_0": "Yadā ca so mato seti,",
  "snp1.11:8.2_0": "Uddhumāto vinīlako;",
  "snp1.11:8.3_0": "Apaviddho susānasmiṁ,",
  "snp1.11:8.4_0": "Anapekkhā honti ñātayo.",
  "snp1.11:9.1_0": "Khādanti naṁ suvānā ca,",
  "snp1.11:9.2_0": "siṅgālā ca vakā kimī;",
  "snp1.11:9.3_0": "Kākā gijjhā ca khādanti,",
  "snp1.11:9.4_0": "ye caññe santi pāṇino.",
  "snp1.11:10.1_0": "Sutvāna buddhavacanaṁ,",
  "snp1.11:10.2_0": "Bhikkhu paññāṇavā idha;",
  "snp1.11:10.3_0": "So kho naṁ parijānāti,",
  "snp1.11:10.4_0": "Yathābhūtañhi passati.",
  "snp1.11:11.1_0": "Yathā idaṁ tathā etaṁ,",
  "snp1.11:11.2_0": "Yathā etaṁ tathā idaṁ;",
  "snp1.11:11.3_0": "Ajjhattañca bahiddhā ca,",
  "snp1.11:11.4_0": "Kāye chandaṁ virājaye.",
  "snp1.11:12.1_0": "Chandarāgaviratto so,",
  "snp1.11:12.2_0": "Bhikkhu paññāṇavā idha;",
  "snp1.11:12.3_0": "Ajjhagā amataṁ santiṁ,",
  "snp1.11:12.4_0": "Nibbānaṁ padamaccutaṁ.",
  "snp1.11:13.1_0": "Dvipādakoyaṁ asuci,",
  "snp1.11:13.2_0": "Duggandho parihīrati;",
  "snp1.11:13.3_0": "Nānākuṇapaparipūro,",
  "snp1.11:13.4_0": "Vissavanto tato tato.",
  "snp1.11:14.1_0": "Etādisena kāyena,",
  "snp1.11:14.2_0": "Yo maññe uṇṇametave;",
  "snp1.11:14.3_0": "Paraṁ vā avajāneyya,",
  "snp1.11:14.4_0": "Kimaññatra adassanāti.",
  "snp1.11:14.5_0": "Vijayasuttaṁ ekādasamaṁ."
}