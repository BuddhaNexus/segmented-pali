{
  "patthana21.35:0.1_0": "Paṭṭhānapakaraṇa",
  "patthana21.35:0.2_0": "Dhammapaccanīyānuloma (4), Dukatikapaṭṭhānapāḷi (3)",
  "patthana21.35:0.3_0": "21.35. Hetuduka, Ajjhattattika",
  "patthana21.35:1.1_0": "Nahetu naajjhatto dhammo hetussa ajjhattassa dhammassa ārammaṇapaccayena paccayo.",
  "patthana21.35:1.1_1": "(Saṅkhittaṁ.) Ārammaṇe nava,",
  "patthana21.35:1.1_2": "adhipatiyā upanissaye nava,",
  "patthana21.35:1.1_3": "purejāte atthiyā avigate tīṇi.",
  "patthana21.35:2.1_0": "Nahetu nabahiddhā dhammo hetussa bahiddhā dhammassa ārammaṇapaccayena paccayo.",
  "patthana21.35:2.1_1": "(Saṅkhittaṁ.) Ārammaṇe nava,",
  "patthana21.35:2.1_2": "adhipatiyā upanissaye nava,",
  "patthana21.35:2.1_3": "purejāte atthiyā avigate tīṇi.",
  "patthana21.35:3.1_0": "(Ajjhattattiko na labbhati paṭiccavārādīsu.)"
}