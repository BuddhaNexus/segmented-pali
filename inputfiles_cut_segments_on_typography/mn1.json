{
  "mn1:0.1_0": "Majjhima Nikāya 1",
  "mn1:0.2_0": "Mūlapariyāyasutta",
  "mn1:1.1_0": "Evaṁ me sutaṁ—",
  "mn1:1.2_0": "ekaṁ samayaṁ bhagavā ukkaṭṭhāyaṁ viharati subhagavane sālarājamūle.",
  "mn1:1.3_0": "Tatra kho bhagavā bhikkhū āmantesi:",
  "mn1:1.4_0": "“bhikkhavo”ti.",
  "mn1:1.5_0": "“Bhadante”ti te bhikkhū bhagavato paccassosuṁ.",
  "mn1:1.6_0": "Bhagavā etadavoca:",
  "mn1:2.1_0": "“sabbadhammamūlapariyāyaṁ vo, bhikkhave, desessāmi.",
  "mn1:2.2_0": "Taṁ suṇātha, sādhukaṁ manasi karotha, bhāsissāmī”ti.",
  "mn1:2.3_0": "“Evaṁ, bhante”ti kho te bhikkhū bhagavato paccassosuṁ.",
  "mn1:2.4_0": "Bhagavā etadavoca:",
  "mn1:3.1_0": "“Idha,",
  "mn1:3.1_1": "bhikkhave,",
  "mn1:3.1_2": "assutavā puthujjano ariyānaṁ adassāvī ariyadhammassa akovido ariyadhamme avinīto,",
  "mn1:3.1_3": "sappurisānaṁ adassāvī sappurisadhammassa akovido sappurisadhamme avinīto—",
  "mn1:3.2_0": "pathaviṁ pathavito sañjānāti;",
  "mn1:3.3_0": "pathaviṁ pathavito saññatvā pathaviṁ maññati,",
  "mn1:3.3_1": "pathaviyā maññati,",
  "mn1:3.3_2": "pathavito maññati,",
  "mn1:3.3_3": "pathaviṁ meti maññati,",
  "mn1:3.3_4": "pathaviṁ abhinandati.",
  "mn1:3.4_0": "Taṁ kissa hetu?",
  "mn1:3.5_0": "‘Apariññātaṁ tassā’ti vadāmi.",
  "mn1:4.1_0": "Āpaṁ āpato sañjānāti;",
  "mn1:4.2_0": "āpaṁ āpato saññatvā āpaṁ maññati,",
  "mn1:4.2_1": "āpasmiṁ maññati,",
  "mn1:4.2_2": "āpato maññati,",
  "mn1:4.2_3": "āpaṁ meti maññati,",
  "mn1:4.2_4": "āpaṁ abhinandati.",
  "mn1:4.3_0": "Taṁ kissa hetu?",
  "mn1:4.4_0": "‘Apariññātaṁ tassā’ti vadāmi.",
  "mn1:5.1_0": "Tejaṁ tejato sañjānāti;",
  "mn1:5.2_0": "tejaṁ tejato saññatvā tejaṁ maññati,",
  "mn1:5.2_1": "tejasmiṁ maññati,",
  "mn1:5.2_2": "tejato maññati,",
  "mn1:5.2_3": "tejaṁ meti maññati,",
  "mn1:5.2_4": "tejaṁ abhinandati.",
  "mn1:5.3_0": "Taṁ kissa hetu?",
  "mn1:5.4_0": "‘Apariññātaṁ tassā’ti vadāmi.",
  "mn1:6.1_0": "Vāyaṁ vāyato sañjānāti;",
  "mn1:6.2_0": "vāyaṁ vāyato saññatvā vāyaṁ maññati,",
  "mn1:6.2_1": "vāyasmiṁ maññati,",
