{
  "vv57:0.1_0": "Vimānavatthu",
  "vv57:0.2_0": "Purisavimāna",
  "vv57:0.3_0": "Mahārathavagga",
  "vv57:0.4_0": "7. Dutiyakaraṇīyavimānavatthu",
  "vv57:1.1_0": "“Uccamidaṁ maṇithūṇaṁ vimānaṁ,",
  "vv57:1.2_0": "Samantato dvādasa yojanāni;",
  "vv57:1.3_0": "Kūṭāgārā sattasatā uḷārā,",
  "vv57:1.4_0": "Veḷuriyathambhā rucakatthatā subhā.",
  "vv57:2.1_0": "Tatthacchasi pivasi khādasi ca,",
  "vv57:2.2_0": "Dibbā ca vīṇā pavadanti vagguṁ;",
  "vv57:2.3_0": "Dibbā rasā kāmaguṇettha pañca,",
  "vv57:2.4_0": "Nāriyo ca naccanti suvaṇṇachannā.",
  "vv57:3.1_0": "Kena tetādiso vaṇṇo,",
  "vv57:3.2_0": "…pe…",
  "vv57:3.3_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv57:4.1_0": "So devaputto attamano,",
  "vv57:4.2_0": "…pe…",
  "vv57:4.3_0": "yassa kammassidaṁ phalaṁ.",
  "vv57:5.1_0": "“Karaṇīyāni puññāni,",
  "vv57:5.2_0": "paṇḍitena vijānatā;",
  "vv57:5.3_0": "Sammaggatesu bhikkhūsu,",
  "vv57:5.4_0": "yattha dinnaṁ mahapphalaṁ.",
  "vv57:6.1_0": "Atthāya vata me bhikkhu,",
  "vv57:6.2_0": "araññā gāmamāgato;",
  "vv57:6.3_0": "Tattha cittaṁ pasādetvā,",
  "vv57:6.4_0": "tāvatiṁsūpago ahaṁ.",
  "vv57:7.1_0": "Tena metādiso vaṇṇo,",
  "vv57:7.2_0": "…pe…",
  "vv57:7.3_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv57:8.1_0": "Dutiyakaraṇīyavimānaṁ sattamaṁ."
}