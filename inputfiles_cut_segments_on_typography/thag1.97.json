{
  "thag1.97:0.1_0": "Theragāthā 1.97",
  "thag1.97:0.2_0": "Ekakanipāta",
  "thag1.97:0.3_0": "Dasamavagga",
  "thag1.97:0.4_0": "Tissattheragāthā",
  "thag1.97:1.1_0": "“Hitvā satapalaṁ kaṁsaṁ,",
  "thag1.97:1.2_0": "Sovaṇṇaṁ satarājikaṁ;",
  "thag1.97:1.3_0": "Aggahiṁ mattikāpattaṁ,",
  "thag1.97:1.4_0": "Idaṁ dutiyābhisecanan”ti.",
  "thag1.97:1.5_0": "… Tisso thero …."
}