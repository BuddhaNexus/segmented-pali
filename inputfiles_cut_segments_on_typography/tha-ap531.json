{
  "tha-ap531:0.1_0": "Therāpadāna",
  "tha-ap531:0.2_0": "Tiṇadāyakavagga",
  "tha-ap531:0.3_0": "9. Āmaṇḍaphaladāyakattheraapadāna",
  "tha-ap531:1.1_0": "“Padumuttaro nāma jino,",
  "tha-ap531:1.2_0": "sabbadhammāna pāragū;",
  "tha-ap531:1.3_0": "Vuṭṭhahitvā samādhimhā,",
  "tha-ap531:1.4_0": "caṅkamī lokanāyako.",
  "tha-ap531:2.1_0": "Khāribhāraṁ gahetvāna,",
  "tha-ap531:2.2_0": "āharanto phalaṁ tadā;",
  "tha-ap531:2.3_0": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap531:2.4_0": "caṅkamantaṁ mahāmuniṁ.",
  "tha-ap531:3.1_0": "Pasannacitto sumano,",
  "tha-ap531:3.2_0": "sire katvāna añjaliṁ;",
  "tha-ap531:3.3_0": "Sambuddhaṁ abhivādetvā,",
  "tha-ap531:3.4_0": "āmaṇḍamadadiṁ phalaṁ.",
  "tha-ap531:4.1_0": "Satasahassito kappe,",
  "tha-ap531:4.2_0": "yaṁ phalaṁ adadiṁ tadā;",
  "tha-ap531:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap531:4.4_0": "āmaṇḍassa idaṁ phalaṁ.",
  "tha-ap531:5.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap531:5.2_0": "…pe…",
  "tha-ap531:5.3_0": "viharāmi anāsavo.",
  "tha-ap531:6.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap531:6.2_0": "…pe…",
  "tha-ap531:6.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap531:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap531:7.2_0": "…pe…",
  "tha-ap531:7.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap531:8.1_0": "Itthaṁ sudaṁ āyasmā āmaṇḍaphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap531:9.1_0": "Āmaṇḍaphaladāyakattherassāpadānaṁ navamaṁ."
}