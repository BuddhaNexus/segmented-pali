{
  "tika-s0303t9:0_0": "10. Gandhabbakāyasaṁyuttavaṇṇanā",
  "tika-s0303t9:1_0": "438- 549.",
  "tika-s0303t9:1_1": "Mūlagandhādibhedaṁ gandhaṁ avanti apayuñjantīti gandhabbā,",
  "tika-s0303t9:1_2": "tesaṁ kāyo samūho gandhabbakāyo,",
  "tika-s0303t9:1_3": "gandhabbadevanikāyo.",
  "tika-s0303t9:1_4": "Cātumahārājikesu ekiyāva te daṭṭhabbā,",
  "tika-s0303t9:1_5": "tappariyāpannatāya tattha vā niyuttāti gandhabbakāyikā.",
  "tika-s0303t9:1_6": "Tesaṁ tesaṁ rukkhagacchalatānaṁ mūlaṁ paṭicca pavatto gandho mūlagandho,",
  "tika-s0303t9:1_7": "tasmiṁ mūlagandhe.",
  "tika-s0303t9:1_8": "Adhivatthāti mūlagandhaṁ adhiṭṭhāya,",
  "tika-s0303t9:1_9": "abhibhuyya vā vasantā.",
  "tika-s0303t9:1_10": "Esa nayo sesesupi.",
  "tika-s0303t9:1_11": "Taṁ nissāyāti taṁ mūlagandhaṁ rukkhaṁ paccayaṁ katvā nibbattā.",
  "tika-s0303t9:1_12": "Na kevalaṁ tattha gandho eva,",
  "tika-s0303t9:1_13": "mūlameva vā tesaṁ paccayoti dassento “so hī”tiādimāha.",
  "tika-s0303t9:1_14": "Upakappatīti nivāsaṭṭhānabhāvena viniyuñjati.",
  "tika-s0303t9:1_15": "Gandhagandheti gandhānaṁ gandhasamudāye.",
  "tika-s0303t9:1_16": "Mūlādigandhānaṁ gandheti mūlādigataavayavagandhānaṁ gandhe,",
  "tika-s0303t9:1_17": "timūlādigatasamudāyabhūteti attho.",
  "tika-s0303t9:1_18": "Pubbe hi “mūlagandhe”tiādinā rukkhānaṁ avayavagandho gahito,",
  "tika-s0303t9:1_19": "idha pana sabbaso gahitattā samudāyagandho veditabbo.",
  "tika-s0303t9:1_20": "Tenāha “yassa hi rukkhassā”tiādi.",
  "tika-s0303t9:1_21": "Soti so sabbo mūlādigato gandho gandhasamudāyo idha gandhagandho nāma.",
  "tika-s0303t9:1_22": "Tassa gandhassa gandheti tassa samudāyagandhassa tathābhūte gandhe.",
  "tika-s0303t9:1_23": "Sarikkhaṁ sadisaṁ paṭidānaṁ etissāti sarikkhadānaṁ, patthanā.",
  "tika-s0303t9:1_24": "Yathādhippetaphalāni sarikkhadānattāva adhippetaphalaṁ dentu,",
  "tika-s0303t9:1_25": "asarikkhadānaṁ kathanti?",
  "tika-s0303t9:1_26": "Tampi detiyeva puññassa sabbakāmadadattāti āha “asarikkhadānampī”tiādi.",
  "tika-s0303t9:2_0": "Gandhabbakāyasaṁyuttavaṇṇanā niṭṭhitā."
}