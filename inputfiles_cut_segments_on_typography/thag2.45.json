{
  "thag2.45:0.1_0": "Theragāthā 2.45",
  "thag2.45:0.2_0": "Dukanipāta",
  "thag2.45:0.3_0": "Pañcamavagga",
  "thag2.45:0.4_0": "Visākhapañcālaputtattheragāthā",
  "thag2.45:1.1_0": "“Na ukkhipe no ca parikkhipe pare,",
  "thag2.45:1.2_0": "Na okkhipe pāragataṁ na eraye;",
  "thag2.45:1.3_0": "Na cattavaṇṇaṁ parisāsu byāhare,",
  "thag2.45:1.4_0": "Anuddhato sammitabhāṇi subbato.",
  "thag2.45:2.1_0": "Susukhumanipuṇatthadassinā,",
  "thag2.45:2.2_0": "Matikusalena nivātavuttinā;",
  "thag2.45:2.3_0": "Saṁsevitavuddhasīlinā,",
  "thag2.45:2.4_0": "Nibbānaṁ na hi tena dullabhan”ti.",
  "thag2.45:2.5_0": "… Visākho pañcālaputto thero …."
}