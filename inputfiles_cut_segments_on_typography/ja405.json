{
  "ja405:0.1_0": "Jātaka",
  "ja405:0.2_0": "Sattakanipāta",
  "ja405:0.3_0": "Kukkuvagga",
  "ja405:0.4_0": "10. Bakajātaka",
  "ja405:1.1_0": "“Dvāsattati gotama puññakammā,",
  "ja405:1.2_0": "Vasavattino jātijaraṁ atītā;",
  "ja405:1.3_0": "Ayamantimā vedagū brahmapatti,",
  "ja405:1.4_0": "Asmābhijappanti janā anekā”.",
  "ja405:2.1_0": "“Appañhi etaṁ na hi dīghamāyu,",
  "ja405:2.2_0": "Yaṁ tvaṁ baka maññasi dīghamāyuṁ;",
  "ja405:2.3_0": "Sataṁ sahassāni nirabbudānaṁ,",
  "ja405:2.4_0": "Āyuṁ pajānāmi tavāha brahme”.",
  "ja405:3.1_0": "“Anantadassī bhagavāhamasmi,",
  "ja405:3.2_0": "Jātijjaraṁ sokamupātivatto;",
  "ja405:3.3_0": "Kiṁ me purāṇaṁ vatasīlavattaṁ,",
  "ja405:3.4_0": "Ācikkha me taṁ yamahaṁ vijaññaṁ”.",
  "ja405:4.1_0": "“Yaṁ tvaṁ apāyesi bahū manusse,",
  "ja405:4.2_0": "Pipāsite ghammani samparete;",
  "ja405:4.3_0": "Taṁ te purāṇaṁ vatasīlavattaṁ,",
  "ja405:4.4_0": "Suttappabuddhova anussarāmi.",
  "ja405:5.1_0": "Yaṁ eṇikūlasmi janaṁ gahītaṁ,",
  "ja405:5.2_0": "Amocayī gayhaka niyyamānaṁ;",
  "ja405:5.3_0": "Taṁ te purāṇaṁ vatasīlavattaṁ,",
  "ja405:5.4_0": "Suttappabuddhova anussarāmi.",
  "ja405:6.1_0": "Gaṅgāya sotasmiṁ gahītanāvaṁ,",
  "ja405:6.2_0": "Luddena nāgena manussakappā;",
  "ja405:6.3_0": "Amocayi tvaṁ balasā pasayha,",
  "ja405:6.4_0": "Taṁ te purāṇaṁ vatasīlavattaṁ;",
  "ja405:6.5_0": "Suttappabuddhova anussarāmi.",
  "ja405:7.1_0": "Kappo ca te baddhacaro ahosiṁ,",
  "ja405:7.2_0": "Sambuddhimantaṁ vatinaṁ amaññaṁ;",
  "ja405:7.3_0": "Taṁ te purāṇaṁ vatasīlavattaṁ,",
  "ja405:7.4_0": "Suttappabuddhova anussarāmi”.",
  "ja405:8.1_0": "“Addhā pajānāsi mametamāyuṁ,",
  "ja405:8.2_0": "Aññampi jānāsi tathā hi buddho;",
  "ja405:8.3_0": "Tathā hi tāyaṁ jalitānubhāvo,",
  "ja405:8.4_0": "Obhāsayaṁ tiṭṭhati brahmalokan”ti.",
  "ja405:9.1_0": "Bakajātakaṁ dasamaṁ.",
  "ja405:9.2_0": "Kukkuvaggo paṭhamo.",
  "ja405:10.1_0": "Tassuddānaṁ",
  "ja405:11.1_0": "Varakaṇṇika cāpavaro sutano,",
  "ja405:11.2_0": "Atha gijjha sarohitamacchavaro;",
  "ja405:11.3_0": "Puna paṇṇaka senaka yācanako,",
  "ja405:11.4_0": "Atha veri sabrahmabakena dasāti."
}