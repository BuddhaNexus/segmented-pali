{
  "snp5.1:0.1_0": "Sutta Nipāta 5.1",
  "snp5.1:0.2_0": "Vatthugāthā",
  "snp5.1:1.1_0": "Kosalānaṁ purā rammā,",
  "snp5.1:1.2_0": "agamā dakkhiṇāpathaṁ;",
  "snp5.1:1.3_0": "Ākiñcaññaṁ patthayāno,",
  "snp5.1:1.4_0": "brāhmaṇo mantapāragū.",
  "snp5.1:2.1_0": "So assakassa visaye,",
  "snp5.1:2.2_0": "aḷakassa samāsane;",
  "snp5.1:2.3_0": "Vasi godhāvarīkūle,",
  "snp5.1:2.4_0": "uñchena ca phalena ca.",
  "snp5.1:3.1_0": "Tasseva upanissāya,",
  "snp5.1:3.2_0": "gāmo ca vipulo ahu;",
  "snp5.1:3.3_0": "Tato jātena āyena,",
  "snp5.1:3.4_0": "mahāyaññamakappayi.",
  "snp5.1:4.1_0": "Mahāyaññaṁ yajitvāna,",
  "snp5.1:4.2_0": "puna pāvisi assamaṁ;",
  "snp5.1:4.3_0": "Tasmiṁ paṭipaviṭṭhamhi,",
  "snp5.1:4.4_0": "añño āgañchi brāhmaṇo.",
  "snp5.1:5.1_0": "Ugghaṭṭapādo tasito,",
  "snp5.1:5.2_0": "paṅkadanto rajassiro;",
  "snp5.1:5.3_0": "So ca naṁ upasaṅkamma,",
  "snp5.1:5.4_0": "satāni pañca yācati.",
  "snp5.1:6.1_0": "Tamenaṁ bāvarī disvā,",
  "snp5.1:6.2_0": "āsanena nimantayi;",
  "snp5.1:6.3_0": "Sukhañca kusalaṁ pucchi,",
  "snp5.1:6.4_0": "idaṁ vacanamabravi.",
  "snp5.1:7.1_0": "“Yaṁ kho mama deyyadhammaṁ,",
  "snp5.1:7.2_0": "Sabbaṁ visajjitaṁ mayā;",
  "snp5.1:7.3_0": "Anujānāhi me brahme,",
  "snp5.1:7.4_0": "Natthi pañcasatāni me”.",
  "snp5.1:8.1_0": "“Sace me yācamānassa,",
  "snp5.1:8.2_0": "bhavaṁ nānupadassati;",
  "snp5.1:8.3_0": "Sattame divase tuyhaṁ,",
  "snp5.1:8.4_0": "muddhā phalatu sattadhā”.",
  "snp5.1:9.1_0": "Abhisaṅkharitvā kuhako,",
  "snp5.1:9.2_0": "bheravaṁ so akittayi;",
  "snp5.1:9.3_0": "Tassa taṁ vacanaṁ sutvā,",
  "snp5.1:9.4_0": "bāvarī dukkhito ahu.",
  "snp5.1:10.1_0": "Ussussati anāhāro,",
  "snp5.1:10.2_0": "Sokasallasamappito;",
  "snp5.1:10.3_0": "Athopi evaṁ cittassa,",
  "snp5.1:10.4_0": "Jhāne na ramatī mano.",
  "snp5.1:11.1_0": "Utrastaṁ dukkhitaṁ disvā,",
  "snp5.1:11.2_0": "Devatā atthakāminī;",
  "snp5.1:11.3_0": "Bāvariṁ upasaṅkamma,",
  "snp5.1:11.4_0": "Idaṁ vacanamabravi.",
  "snp5.1:12.1_0": "“Na so muddhaṁ pajānāti,",
  "snp5.1:12.2_0": "Kuhako so dhanatthiko;",
  "snp5.1:12.3_0": "Muddhani muddhapāte vā,",
  "snp5.1:12.4_0": "Ñāṇaṁ tassa na vijjati”.",
  "snp5.1:13.1_0": "“Bhotī carahi jānāsi,",
  "snp5.1:13.2_0": "Taṁ me akkhāhi pucchitā;",
  "snp5.1:13.3_0": "Muddhaṁ muddhādhipātañca,",
  "snp5.1:13.4_0": "Taṁ suṇoma vaco tava”.",
  "snp5.1:14.1_0": "“Ahampetaṁ na jānāmi,",
  "snp5.1:14.2_0": "Ñāṇamettha na vijjati;",
  "snp5.1:14.3_0": "Muddhani muddhādhipāte ca,",
  "snp5.1:14.4_0": "Jinānaṁ hettha dassanaṁ”.",
  "snp5.1:15.1_0": "“Atha ko carahi jānāti,",
  "snp5.1:15.2_0": "Asmiṁ pathavimaṇḍale;",
  "snp5.1:15.3_0": "Muddhaṁ muddhādhipātañca,",
  "snp5.1:15.4_0": "Taṁ me akkhāhi devate”.",
  "snp5.1:16.1_0": "“Purā kapilavatthumhā,",
  "snp5.1:16.2_0": "Nikkhanto lokanāyako;",
  "snp5.1:16.3_0": "Apacco okkākarājassa,",
  "snp5.1:16.4_0": "Sakyaputto pabhaṅkaro.",
  "snp5.1:17.1_0": "So hi brāhmaṇa sambuddho,",
  "snp5.1:17.2_0": "Sabbadhammāna pāragū;",
  "snp5.1:17.3_0": "Sabbābhiññābalappatto,",
  "snp5.1:17.4_0": "Sabbadhammesu cakkhumā;",
  "snp5.1:17.5_0": "Sabbakammakkhayaṁ patto,",
  "snp5.1:17.6_0": "Vimutto upadhikkhaye.",
  "snp5.1:18.1_0": "Buddho so bhagavā loke,",
  "snp5.1:18.2_0": "Dhammaṁ deseti cakkhumā;",
  "snp5.1:18.3_0": "Taṁ tvaṁ gantvāna pucchassu,",
  "snp5.1:18.4_0": "So te taṁ byākarissati”.",
  "snp5.1:19.1_0": "Sambuddhoti vaco sutvā,",
  "snp5.1:19.2_0": "Udaggo bāvarī ahu;",
  "snp5.1:19.3_0": "Sokassa tanuko āsi,",
  "snp5.1:19.4_0": "Pītiñca vipulaṁ labhi.",
  "snp5.1:20.1_0": "So bāvarī attamano udaggo,",
  "snp5.1:20.2_0": "Taṁ devataṁ pucchati vedajāto;",
  "snp5.1:20.3_0": "“Katamamhi gāme nigamamhi vā pana,",
  "snp5.1:20.4_0": "Katamamhi vā janapade lokanātho;",
  "snp5.1:20.5_0": "Yattha gantvāna passemu,",
  "snp5.1:20.6_0": "Sambuddhaṁ dvipaduttamaṁ”.",
  "snp5.1:21.1_0": "“Sāvatthiyaṁ kosalamandire jino,",
  "snp5.1:21.2_0": "Pahūtapañño varabhūrimedhaso;",
  "snp5.1:21.3_0": "So sakyaputto vidhuro anāsavo,",
  "snp5.1:21.4_0": "Muddhādhipātassa vidū narāsabho”.",
  "snp5.1:22.1_0": "Tato āmantayī sisse,",
  "snp5.1:22.2_0": "Brāhmaṇe mantapārage;",
  "snp5.1:22.3_0": "“Etha māṇavā akkhissaṁ,",
  "snp5.1:22.4_0": "Suṇātha vacanaṁ mama.",
  "snp5.1:23.1_0": "Yasseso dullabho loke,",
  "snp5.1:23.2_0": "Pātubhāvo abhiṇhaso;",
  "snp5.1:23.3_0": "Svājja lokamhi uppanno,",
  "snp5.1:23.4_0": "Sambuddho iti vissuto;",
  "snp5.1:23.5_0": "Khippaṁ gantvāna sāvatthiṁ,",
  "snp5.1:23.6_0": "Passavho dvipaduttamaṁ”.",
  "snp5.1:24.1_0": "“Kathaṁ carahi jānemu,",
  "snp5.1:24.2_0": "Disvā buddhoti brāhmaṇa;",
  "snp5.1:24.3_0": "Ajānataṁ no pabrūhi,",
  "snp5.1:24.4_0": "Yathā jānemu taṁ mayaṁ”.",
  "snp5.1:25.1_0": "“Āgatāni hi mantesu,",
  "snp5.1:25.2_0": "Mahāpurisalakkhaṇā;",
  "snp5.1:25.3_0": "Dvattiṁsāni ca byākkhātā,",
  "snp5.1:25.4_0": "Samattā anupubbaso.",
  "snp5.1:26.1_0": "Yassete honti gattesu,",
  "snp5.1:26.2_0": "Mahāpurisalakkhaṇā;",
  "snp5.1:26.3_0": "Dveyeva tassa gatiyo,",
  "snp5.1:26.4_0": "Tatiyā hi na vijjati.",
  "snp5.1:27.1_0": "Sace agāraṁ āvasati,",
  "snp5.1:27.2_0": "Vijeyya pathaviṁ imaṁ;",
  "snp5.1:27.3_0": "Adaṇḍena asatthena,",
  "snp5.1:27.4_0": "Dhammena manusāsati.",
  "snp5.1:28.1_0": "Sace ca so pabbajati,",
  "snp5.1:28.2_0": "Agārā anagāriyaṁ;",
  "snp5.1:28.3_0": "Vivaṭṭacchado sambuddho,",
  "snp5.1:28.4_0": "Arahā bhavati anuttaro.",
  "snp5.1:29.1_0": "Jātiṁ gottañca lakkhaṇaṁ,",
  "snp5.1:29.2_0": "Mante sisse punāpare;",
  "snp5.1:29.3_0": "Muddhaṁ muddhādhipātañca,",
  "snp5.1:29.4_0": "Manasāyeva pucchatha.",
  "snp5.1:30.1_0": "Anāvaraṇadassāvī,",
  "snp5.1:30.2_0": "Yadi buddho bhavissati;",
  "snp5.1:30.3_0": "Manasā pucchite pañhe,",
  "snp5.1:30.4_0": "Vācāya vissajessati”.",
  "snp5.1:31.1_0": "Bāvarissa vaco sutvā,",
  "snp5.1:31.2_0": "Sissā soḷasa brāhmaṇā;",
  "snp5.1:31.3_0": "Ajito tissametteyyo,",
  "snp5.1:31.4_0": "Puṇṇako atha mettagū.",
  "snp5.1:32.1_0": "Dhotako upasīvo ca,",
  "snp5.1:32.2_0": "Nando ca atha hemako;",
  "snp5.1:32.3_0": "Todeyya-kappā dubhayo,",
  "snp5.1:32.4_0": "Jatukaṇṇī ca paṇḍito.",
  "snp5.1:33.1_0": "Bhadrāvudho udayo ca,",
  "snp5.1:33.2_0": "Posālo cāpi brāhmaṇo;",
  "snp5.1:33.3_0": "Mogharājā ca medhāvī,",
  "snp5.1:33.4_0": "Piṅgiyo ca mahāisi.",
  "snp5.1:34.1_0": "Paccekagaṇino sabbe,",
  "snp5.1:34.2_0": "Sabbalokassa vissutā;",
  "snp5.1:34.3_0": "Jhāyī jhānaratā dhīrā,",
  "snp5.1:34.4_0": "Pubbavāsanavāsitā.",
  "snp5.1:35.1_0": "Bāvariṁ abhivādetvā,",
  "snp5.1:35.2_0": "Katvā ca naṁ padakkhiṇaṁ;",
  "snp5.1:35.3_0": "Jaṭājinadharā sabbe,",
  "snp5.1:35.4_0": "Pakkāmuṁ uttarāmukhā.",
  "snp5.1:36.1_0": "Aḷakassa patiṭṭhānaṁ,",
  "snp5.1:36.2_0": "Purimāhissatiṁ tadā;",
  "snp5.1:36.3_0": "Ujjeniñcāpi gonaddhaṁ,",
  "snp5.1:36.4_0": "Vedisaṁ vanasavhayaṁ.",
  "snp5.1:37.1_0": "Kosambiñcāpi sāketaṁ,",
  "snp5.1:37.2_0": "Sāvatthiñca puruttamaṁ;",
  "snp5.1:37.3_0": "Setabyaṁ kapilavatthuṁ,",
  "snp5.1:37.4_0": "Kusinārañca mandiraṁ.",
  "snp5.1:38.1_0": "Pāvañca bhoganagaraṁ,",
  "snp5.1:38.2_0": "Vesāliṁ māgadhaṁ puraṁ;",
  "snp5.1:38.3_0": "Pāsāṇakaṁ cetiyañca,",
  "snp5.1:38.4_0": "Ramaṇīyaṁ manoramaṁ.",
  "snp5.1:39.1_0": "Tasitovudakaṁ sītaṁ,",
  "snp5.1:39.2_0": "Mahālābhaṁva vāṇijo;",
  "snp5.1:39.3_0": "Chāyaṁ ghammābhitattova,",
  "snp5.1:39.4_0": "Turitā pabbatamāruhuṁ.",
  "snp5.1:40.1_0": "Bhagavā tamhi samaye,",
  "snp5.1:40.2_0": "Bhikkhusaṅghapurakkhato;",
  "snp5.1:40.3_0": "Bhikkhūnaṁ dhammaṁ deseti,",
  "snp5.1:40.4_0": "Sīhova nadatī vane.",
  "snp5.1:41.1_0": "Ajito addasa buddhaṁ,",
  "snp5.1:41.2_0": "Sataraṁsiṁva bhāṇumaṁ;",
  "snp5.1:41.3_0": "Candaṁ yathā pannarase,",
  "snp5.1:41.4_0": "Pāripūriṁ upāgataṁ.",
  "snp5.1:42.1_0": "Athassa gatte disvāna,",
  "snp5.1:42.2_0": "Paripūrañca byañjanaṁ;",
  "snp5.1:42.3_0": "Ekamantaṁ ṭhito haṭṭho,",
  "snp5.1:42.4_0": "Manopañhe apucchatha.",
  "snp5.1:43.1_0": "“Ādissa jammanaṁ brūhi,",
  "snp5.1:43.2_0": "Gottaṁ brūhi salakkhaṇaṁ;",
  "snp5.1:43.3_0": "Mantesu pāramiṁ brūhi,",
  "snp5.1:43.4_0": "Kati vāceti brāhmaṇo”.",
  "snp5.1:44.1_0": "“Vīsaṁ vassasataṁ āyu,",
  "snp5.1:44.2_0": "So ca gottena bāvarī;",
  "snp5.1:44.3_0": "Tīṇissa lakkhaṇā gatte,",
  "snp5.1:44.4_0": "Tiṇṇaṁ vedāna pāragū.",
  "snp5.1:45.1_0": "Lakkhaṇe itihāse ca,",
  "snp5.1:45.2_0": "Sanighaṇḍusakeṭubhe;",
  "snp5.1:45.3_0": "Pañcasatāni vāceti,",
  "snp5.1:45.4_0": "Sadhamme pāramiṁ gato”.",
  "snp5.1:46.1_0": "“Lakkhaṇānaṁ pavicayaṁ,",
  "snp5.1:46.2_0": "Bāvarissa naruttama;",
  "snp5.1:46.3_0": "Kaṅkhacchida pakāsehi,",
  "snp5.1:46.4_0": "Mā no kaṅkhāyitaṁ ahu”.",
  "snp5.1:47.1_0": "“Mukhaṁ jivhāya chādeti,",
  "snp5.1:47.2_0": "Uṇṇassa bhamukantare;",
  "snp5.1:47.3_0": "Kosohitaṁ vatthaguyhaṁ,",
  "snp5.1:47.4_0": "Evaṁ jānāhi māṇava”.",
  "snp5.1:48.1_0": "Pucchañhi kiñci asuṇanto,",
  "snp5.1:48.2_0": "Sutvā pañhe viyākate;",
  "snp5.1:48.3_0": "Vicinteti jano sabbo,",
  "snp5.1:48.4_0": "Vedajāto katañjalī.",
  "snp5.1:49.1_0": "“Ko nu devo vā brahmā vā,",
  "snp5.1:49.2_0": "Indo vāpi sujampati;",
  "snp5.1:49.3_0": "Manasā pucchite pañhe,",
  "snp5.1:49.4_0": "Kametaṁ paṭibhāsati”.",
  "snp5.1:50.1_0": "“Muddhaṁ muddhādhipātañca,",
  "snp5.1:50.2_0": "Bāvarī paripucchati;",
  "snp5.1:50.3_0": "Taṁ byākarohi bhagavā,",
  "snp5.1:50.4_0": "Kaṅkhaṁ vinaya no ise”.",
  "snp5.1:51.1_0": "“Avijjā muddhāti jānāhi,",
  "snp5.1:51.2_0": "Vijjā muddhādhipātinī;",
  "snp5.1:51.3_0": "Saddhāsatisamādhīhi,",
  "snp5.1:51.4_0": "Chandaviriyena saṁyutā”.",
  "snp5.1:52.1_0": "Tato vedena mahatā,",
  "snp5.1:52.2_0": "Santhambhitvāna māṇavo;",
  "snp5.1:52.3_0": "Ekaṁsaṁ ajinaṁ katvā,",
  "snp5.1:52.4_0": "Pādesu sirasā pati.",
  "snp5.1:53.1_0": "“Bāvarī brāhmaṇo bhoto,",
  "snp5.1:53.2_0": "Saha sissehi mārisa;",
  "snp5.1:53.3_0": "Udaggacitto sumano,",
  "snp5.1:53.4_0": "Pāde vandati cakkhuma”.",
  "snp5.1:54.1_0": "“Sukhito bāvarī hotu,",
  "snp5.1:54.2_0": "Saha sissehi brāhmaṇo;",
  "snp5.1:54.3_0": "Tvañcāpi sukhito hohi,",
  "snp5.1:54.4_0": "Ciraṁ jīvāhi māṇava.",
  "snp5.1:55.1_0": "Bāvarissa ca tuyhaṁ vā,",
  "snp5.1:55.2_0": "Sabbesaṁ sabbasaṁsayaṁ;",
  "snp5.1:55.3_0": "Katāvakāsā pucchavho,",
  "snp5.1:55.4_0": "Yaṁ kiñci manasicchatha”.",
  "snp5.1:56.1_0": "Sambuddhena katokāso,",
  "snp5.1:56.2_0": "Nisīditvāna pañjalī;",
  "snp5.1:56.3_0": "Ajito paṭhamaṁ pañhaṁ,",
  "snp5.1:56.4_0": "Tattha pucchi tathāgataṁ.",
  "snp5.1:56.5_0": "Vatthugāthā niṭṭhitā."
}