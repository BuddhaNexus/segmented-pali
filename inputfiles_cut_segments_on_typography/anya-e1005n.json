{
  "anya-e1005n:0_0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1005n:1_0": "Lokanīti",
  "anya-e1005n:2_0": "1. Paṇḍitakaṇḍo",
  "anya-e1005n:3_0": "1.",
  "anya-e1005n:4_0": "Lokanītiṃ pavakkhāmi,",
  "anya-e1005n:5_0": "Nānāsatthasamuddhaṭaṃ;",
  "anya-e1005n:6_0": "Māgadheneva saṅkhepaṃ,",
  "anya-e1005n:7_0": "Vanditvā ratanatthayaṃ.",
  "anya-e1005n:8_0": "2.",
  "anya-e1005n:9_0": "Nītiloke purisassa sāro,",
  "anya-e1005n:10_0": "Mātā pitā ācariyoca mitto;",
  "anya-e1005n:11_0": "Tasmā hi nītiṃ puriso vijaññā,",
  "anya-e1005n:12_0": "Ñāṇī mahā hoti bahussutoca.",
  "anya-e1005n:13_0": "3.",
  "anya-e1005n:14_0": "Alasassa kuto sippaṃ,",
  "anya-e1005n:15_0": "Asippassa kuto dhanaṃ;",
  "anya-e1005n:16_0": "Adhanassa kuto mittaṃ,",
  "anya-e1005n:17_0": "Amittassa kuto sukhaṃ;",
  "anya-e1005n:18_0": "Asukhassa kuto puññaṃ,",
  "anya-e1005n:19_0": "Apuññassa kuto varaṃ.",
  "anya-e1005n:20_0": "4.",
  "anya-e1005n:21_0": "Sippā samaṃ dhanaṃ natthi,",
  "anya-e1005n:22_0": "Sippaṃ corā nagaṇhare;",
  "anya-e1005n:23_0": "Idha loke sippaṃ mittaṃ,",
  "anya-e1005n:24_0": "Paraloke sukhāvahaṃ.",
  "anya-e1005n:25_0": "5.",
  "anya-e1005n:26_0": "Appakaṃ nātimaññeyya,",
  "anya-e1005n:27_0": "Citte sutaṃ nidhāpaye;",
  "anya-e1005n:28_0": "Vammikodakabindūva,",
  "anya-e1005n:29_0": "Cirena paripūrati.",
  "anya-e1005n:30_0": "6.",
  "anya-e1005n:31_0": "Khuddoti nātimaññeyya,",
  "anya-e1005n:32_0": "Vijjaṃ vā sippameva vā;",
  "anya-e1005n:33_0": "Ekampi pariyodātaṃ,",
  "anya-e1005n:34_0": "Jīvitakappakāraṇaṃ.",
  "anya-e1005n:35_0": "7.",
  "anya-e1005n:36_0": "Sele sele na māṇikaṃ,",
  "anya-e1005n:37_0": "Gaje gaje na muttikaṃ;",
  "anya-e1005n:38_0": "Vane vane na candanaṃ,",
  "anya-e1005n:39_0": "Ṭhāne ṭhāne na paṇḍitaṃ.",
  "anya-e1005n:40_0": "8.",
  "anya-e1005n:41_0": "Paṇḍito sutasampanno,",
  "anya-e1005n:42_0": "Yattha atthīti ce suto;",
  "anya-e1005n:43_0": "Mahussāhena taṃ ṭhānaṃ,",
  "anya-e1005n:44_0": "Gantabbaṃva sutesinā.",
  "anya-e1005n:45_0": "9.",
  "anya-e1005n:46_0": "Sine sippaṃ sine dhanaṃ,",
  "anya-e1005n:47_0": "Sine pabbatamāruhaṃ;",
  "anya-e1005n:48_0": "Sine kāmassa kodhassa,",
  "anya-e1005n:49_0": "Ime pañca sine sine.",
  "anya-e1005n:50_0": "10.",
  "anya-e1005n:51_0": "Suti sammuti saṅkhyāca,",
  "anya-e1005n:52_0": "Yogā nīti visesakā;",
  "anya-e1005n:53_0": "Gandhabbā gaṇikā ceva,",
  "anya-e1005n:54_0": "Dhanu bedā ca pūraṇā.",
  "anya-e1005n:55_0": "11.",
  "anya-e1005n:56_0": "Tikicchā itihāsā ca,",
  "anya-e1005n:57_0": "Joti māyā ca chandati;",
  "anya-e1005n:58_0": "Ketu mantā ca saddā ca,",
  "anya-e1005n:59_0": "Sippāṭṭhārasakā ime.",
  "anya-e1005n:60_0": "12.",
  "anya-e1005n:61_0": "Apuṭṭho paṇḍito bherī,",
  "anya-e1005n:62_0": "Pajjunno me hoti pucchito;",
  "anya-e1005n:63_0": "Bālo puṭṭho apuṭṭhopi,",
  "anya-e1005n:64_0": "Bahumpi bhaṇate sadā.",
  "anya-e1005n:65_0": "13.",
  "anya-e1005n:66_0": "Potthakesu ca yaṃ sippaṃ,",
  "anya-e1005n:67_0": "Parahatthesu yaṃ dhanaṃ;",
  "anya-e1005n:68_0": "Yathākicce samuppanne,",
  "anya-e1005n:69_0": "Na taṃ sippaṃ na taṃ dhanaṃ.",
  "anya-e1005n:70_0": "14.",
  "anya-e1005n:71_0": "Jalappamāṇaṃ kumuddanālaṃ,",
  "anya-e1005n:72_0": "Kulappamāṇaṃ vinayo pamāṇaṃ;",
  "anya-e1005n:73_0": "Byattippamāṇaṃ kathītavākyaṃ,",
  "anya-e1005n:74_0": "Pathaviyā pamāṇaṃ tiṇa milātaṃ.",
  "anya-e1005n:75_0": "15.",
  "anya-e1005n:76_0": "Appassuto sutaṃ appaṃ,",
  "anya-e1005n:77_0": "Bahuṃ maññati mānavā;",
  "anya-e1005n:78_0": "Sindhūdakaṃ apassanto,",
  "anya-e1005n:79_0": "Kūpe toyaṃva maṇḍuko.",
  "anya-e1005n:80_0": "16.",
  "anya-e1005n:81_0": "Pathamaṃ parājaye sippaṃ,",
  "anya-e1005n:82_0": "Dutiyaṃ parājaye dhanaṃ;",
  "anya-e1005n:83_0": "Tatiyaṃ parājaye dhammaṃ,",
  "anya-e1005n:84_0": "Catutthaṃ kiṃ karissati.",
  "anya-e1005n:85_0": "17.",
  "anya-e1005n:86_0": "Byatta putra kimalaso,",
  "anya-e1005n:87_0": "Abyatto bhārahārako;",
  "anya-e1005n:88_0": "Byattako pūjito loke,",
  "anya-e1005n:89_0": "Byatta putra dine dine.",
  "anya-e1005n:90_0": "18.",
  "anya-e1005n:91_0": "Mātā verī pitā satru,",
  "anya-e1005n:92_0": "Kena bāle na sikkhitā;",
  "anya-e1005n:93_0": "Sabhāmajjhe na sobhanti,",
  "anya-e1005n:94_0": "Haṃsamajjhe bakoyathā.",
  "anya-e1005n:95_0": "19.",
  "anya-e1005n:96_0": "Kaṇṭakaṃ giri ko tikkhati,",
  "anya-e1005n:97_0": "Ko añjanaṃ migakkhikaṃ;",
  "anya-e1005n:98_0": "Uppathaṃ pallale ko sugandhaṃ,",
  "anya-e1005n:99_0": "Kula-putta-rūpo ko pavattati;",
  "anya-e1005n:100_0": "Sāmaṃ-bhāvo.",
  "anya-e1005n:101_0": "20.",
  "anya-e1005n:102_0": "Na rasaṃ akotambulaṃ,",
  "anya-e1005n:103_0": "Adhanassa, laṅkatampi;",
  "anya-e1005n:104_0": "Alonakantu byañjanaṃ,",
  "anya-e1005n:105_0": "Byākaraṇaṃ asippassa.",
  "anya-e1005n:106_0": "21.",
  "anya-e1005n:107_0": "Sussusā sutasampanno,",
  "anya-e1005n:108_0": "Sutāpaññāya pavaḍḍhati;",
  "anya-e1005n:109_0": "Paññāya atthaṃ jānāti,",
  "anya-e1005n:110_0": "Ñāto attho sukhāvaho.",
  "anya-e1005n:111_0": "22.",
  "anya-e1005n:112_0": "Bhojanaṃ methunaṃ niddā,",
  "anya-e1005n:113_0": "Goṇe posepi vijjati;",
  "anya-e1005n:114_0": "Vijjā viseso posassa,",
  "anya-e1005n:115_0": "Taṃ hīno gosamo bhave.",
  "anya-e1005n:116_0": "23.",
  "anya-e1005n:117_0": "Natthi vijjāsamaṃmittaṃ,",
  "anya-e1005n:118_0": "Naca byādhisamo ripu;",
  "anya-e1005n:119_0": "Naca attasamaṃ pemaṃ,",
  "anya-e1005n:120_0": "Naca kammasamaṃ balaṃ.",
  "anya-e1005n:121_0": "24.",
  "anya-e1005n:122_0": "Haṃso majjhe na kākānaṃ,",
  "anya-e1005n:123_0": "Sīho gunnaṃ na sobhate;",
  "anya-e1005n:124_0": "Gadrabhamajjhe turaṅgo,",
  "anya-e1005n:125_0": "Bālamajjhe ca paṇḍito.",
  "anya-e1005n:126_0": "25.",
  "anya-e1005n:127_0": "Yāvajīvampi ce bālo,",
  "anya-e1005n:128_0": "Paṇḍitaṃ payirupāsati;",
  "anya-e1005n:129_0": "Na so dhammaṃ vijānāti,",
  "anya-e1005n:130_0": "Dabbi sūparasaṃ yathā.",
  "anya-e1005n:131_0": "26.",
  "anya-e1005n:132_0": "Muhuttamapi ce viññū,",
  "anya-e1005n:133_0": "Paṇḍitaṃ payirupāsati;",
  "anya-e1005n:134_0": "Khippaṃ dhammaṃ vijānāti,",
  "anya-e1005n:135_0": "Jivhā sūparasaṃ yathā.",
  "anya-e1005n:136_0": "27.",
  "anya-e1005n:137_0": "Vinā satthaṃ na gaccheyya,",
  "anya-e1005n:138_0": "Sūro saṅgāmabhūmiyaṃ;",
  "anya-e1005n:139_0": "Paṇḍitvāddhagū vāṇijo,",
  "anya-e1005n:140_0": "Videsagamano tathā.",
  "anya-e1005n:141_0": "28.",
  "anya-e1005n:142_0": "Dhananāsaṃ manotāpaṃ,",
  "anya-e1005n:143_0": "Ghare duccaritāni ca;",
  "anya-e1005n:144_0": "Vañcanañca avamānaṃ,",
  "anya-e1005n:145_0": "Paṇḍito na pakāsaye.",
  "anya-e1005n:146_0": "29.",
  "anya-e1005n:147_0": "Pattānurūpakaṃ vākyaṃ,",
  "anya-e1005n:148_0": "Sabhāvarūpakaṃ piyaṃ;",
  "anya-e1005n:149_0": "Attānurūpakaṃ kodhaṃ,",
  "anya-e1005n:150_0": "Yo jānāti sa paṇḍito.",
  "anya-e1005n:151_0": "30.",
  "anya-e1005n:152_0": "A-dhanassa rasaṃ khādā,",
  "anya-e1005n:153_0": "A-balassa hathā narā;",
  "anya-e1005n:154_0": "A-paññassa vākya-kathā,",
  "anya-e1005n:155_0": "Ummattaka-samā ime.",
  "anya-e1005n:156_0": "31.",
  "anya-e1005n:157_0": "Anavhāyaṃ gamayanto,",
  "anya-e1005n:158_0": "A-pucchā bahu-bhāsako;",
  "anya-e1005n:159_0": "Atta-guṇaṃ pakāsento,",
  "anya-e1005n:160_0": "Ti-vidhaṃ hīna-lakkhaṇaṃ.",
  "anya-e1005n:161_0": "32.",
  "anya-e1005n:162_0": "Appa-rūpo bahuṃ bhāso,",
  "anya-e1005n:163_0": "Appa-pañño pakāsito;",
  "anya-e1005n:164_0": "Appa-pūro ghaṭo khobhe,",
  "anya-e1005n:165_0": "Appa-khīrā gāvī cathe.",
  "anya-e1005n:166_0": "33.",
  "anya-e1005n:167_0": "Maṇḍūkepi ukre sīhe,",
  "anya-e1005n:168_0": "Kākaggahe piye piye;",
  "anya-e1005n:169_0": "A-paṇḍīpi paṇḍī hutvā,",
  "anya-e1005n:170_0": "Dhīrā pucche vaye vaye.",
  "anya-e1005n:171_0": "34.",
  "anya-e1005n:172_0": "Maṇḍūkepi ukre sīhe,",
  "anya-e1005n:173_0": "Sūkarepi uhe dīpe;",
  "anya-e1005n:174_0": "Biḷāre sadise byagghe,",
  "anya-e1005n:175_0": "Sabba dhīre sippa-same.",
  "anya-e1005n:176_0": "35.",
  "anya-e1005n:177_0": "Na titti rājā dhanamhi,",
  "anya-e1005n:178_0": "Paṇḍitopi su-bhāsite;",
  "anya-e1005n:179_0": "Cakkhuṃpi piya-dassane,",
  "anya-e1005n:180_0": "Jale sāgaro na titti.",
  "anya-e1005n:181_0": "36.",
  "anya-e1005n:182_0": "Rūpa-yobbana-sampannā,",
  "anya-e1005n:183_0": "Visātha-kutha-sambhavā;",
  "anya-e1005n:184_0": "Vijjā-hīnā na sobhanti,",
  "anya-e1005n:185_0": "Nigandhā iva kiṃsukā.",
  "anya-e1005n:186_0": "37.",
  "anya-e1005n:187_0": "Hīne putto rājāmacco,",
  "anya-e1005n:188_0": "Bāla-putto ca paṇḍito;",
  "anya-e1005n:189_0": "A-dhanassa dhanaṃ bahu,",
  "anya-e1005n:190_0": "Purisānaṃ na maññatha.",
  "anya-e1005n:191_0": "38.",
  "anya-e1005n:192_0": "Yo sippa-lobhena,",
  "anya-e1005n:193_0": "Bahuṃ gaṇhāti taṃ sippaṃ;",
  "anya-e1005n:194_0": "Mūgova supinaṃ passaṃ,",
  "anya-e1005n:195_0": "Kathetumpi na ussahe.",
  "anya-e1005n:196_0": "39.",
  "anya-e1005n:197_0": "Bhijjetuṃ kumbhakāro,",
  "anya-e1005n:198_0": "Sobhetuṃ kumbha ghaṭṭati;",
  "anya-e1005n:199_0": "Na khipituṃ apāyesu,",
  "anya-e1005n:200_0": "Sissānaṃ vuḍḍhi-kāraṇā.",
  "anya-e1005n:201_0": "40.",
  "anya-e1005n:202_0": "Taggarañca palāsena,",
  "anya-e1005n:203_0": "Yo naro upanayhati;",
  "anya-e1005n:204_0": "Pattāpi surabhi vāyanti,",
  "anya-e1005n:205_0": "Evaṃ dhīrūpasevanā.",
  "anya-e1005n:206_0": "Paṇḍitakaṇḍo niṭṭhito.",
  "anya-e1005n:207_0": "Sujanakaṇḍo",
  "anya-e1005n:208_0": "41.",
  "anya-e1005n:209_0": "Sabbhireva samāsetha,",
  "anya-e1005n:210_0": "Sabbhi kubbetha santhavaṃ;",
  "anya-e1005n:211_0": "Sataṃ saddhammamaññāya,",
  "anya-e1005n:212_0": "Seyyo hoti na pāpiyo.",
  "anya-e1005n:213_0": "42.",
  "anya-e1005n:214_0": "Caja dujjana saṃsaggaṃ,",
  "anya-e1005n:215_0": "Bhaja sādhu samāgamaṃ;",
  "anya-e1005n:216_0": "Kara puññamahorattiṃ,",
  "anya-e1005n:217_0": "Sara niccamaniccataṃ.",
  "anya-e1005n:218_0": "43.",
  "anya-e1005n:219_0": "Yathā udumbarapakkā,",
  "anya-e1005n:220_0": "Bahirattakamevaca;",
  "anya-e1005n:221_0": "Anto kimīhi sampuṇṇā,",
  "anya-e1005n:222_0": "Evaṃ dujjanahaddayā.",
  "anya-e1005n:223_0": "44.",
  "anya-e1005n:224_0": "Yathāpi panasāpakkā,",
  "anya-e1005n:225_0": "Bahi kaṇḍakameva ca;",
  "anya-e1005n:226_0": "Anto amatasampannā,",
  "anya-e1005n:227_0": "Evaṃ sujanahadayā.",
  "anya-e1005n:228_0": "45.",
  "anya-e1005n:229_0": "Sukkhopi candanataru na jahāti gandhaṃ,",
  "anya-e1005n:230_0": "Nāgo gato naramukhe na jahāti līḷaṃ;",
  "anya-e1005n:231_0": "Yantāgato madhurasaṃ na jahāti ucchu,",
  "anya-e1005n:232_0": "Dukkhopi paṇḍitajano na jahāti dhammaṃ.",
  "anya-e1005n:233_0": "46.",
  "anya-e1005n:234_0": "Sīho nāma jighacchāpi,",
  "anya-e1005n:235_0": "Paṇṇādīni na khādati;",
  "anya-e1005n:236_0": "Sīho nāma kisocāpi,",
  "anya-e1005n:237_0": "Nāgamaṃsaṃ na khādati.",
  "anya-e1005n:238_0": "47.",
  "anya-e1005n:239_0": "Kula-jāto kula-putto,",
  "anya-e1005n:240_0": "Kula-vaṃsa-surakkhato;",
  "anya-e1005n:241_0": "Attanā dukkha-pattopi,",
  "anya-e1005n:242_0": "Hīna-kammaṃ na kāraye.",
  "anya-e1005n:243_0": "48.",
  "anya-e1005n:244_0": "Candanaṃ sītalaṃ loke,",
  "anya-e1005n:245_0": "Tato candaṃva sītalaṃ;",
  "anya-e1005n:246_0": "Canda-candanasītamhā,",
  "anya-e1005n:247_0": "Sādhu vākyaṃ subhāsitaṃ.",
  "anya-e1005n:248_0": "49.",
  "anya-e1005n:249_0": "Udeyya bhāṇu pacchime,",
  "anya-e1005n:250_0": "Merurājā nameyyapi;",
  "anya-e1005n:251_0": "Sītalā narakaggipi,",
  "anya-e1005n:252_0": "Pabbatagge ca uppalaṃ.",
  "anya-e1005n:253_0": "Vikase na viparītaṃ,",
  "anya-e1005n:254_0": "Sādhuvāyyaṃ kudācanaṃ.",
  "anya-e1005n:255_0": "50.",
  "anya-e1005n:256_0": "Sukhā rukkhassa chāyāva,",
  "anya-e1005n:257_0": "Tato ñāti mātā pitu;",
  "anya-e1005n:258_0": "Tato ācariyo rañño,",
  "anya-e1005n:259_0": "Tato buddhassanekadhā.",
  "anya-e1005n:260_0": "51.",
  "anya-e1005n:261_0": "Bhamarā pupphamicchanti,",
  "anya-e1005n:262_0": "Guṇamicchanti sujanā;",
  "anya-e1005n:263_0": "Makkhikā pūtimicchanti,",
  "anya-e1005n:264_0": "Dosamicchanti dujjanā.",
  "anya-e1005n:265_0": "52.",
  "anya-e1005n:266_0": "Mātāhīnassa dubbhāsā,",
  "anya-e1005n:267_0": "Pitāhīnassa dukriyā;",
  "anya-e1005n:268_0": "Ubho mātā pitā hīnā,",
  "anya-e1005n:269_0": "Dubbhasāca dukīriyā.",
  "anya-e1005n:270_0": "53.",
  "anya-e1005n:271_0": "Mātā seṭṭhassa subhāsā,",
  "anya-e1005n:272_0": "Pitā seṭṭhassa sukriyā;",
  "anya-e1005n:273_0": "Ubho mātā pitā seṭṭhā,",
  "anya-e1005n:274_0": "Subhāsāca sukīriyā.",
  "anya-e1005n:275_0": "54.",
  "anya-e1005n:276_0": "Saṅgāme sūra-micchanti,",
  "anya-e1005n:277_0": "Mantīsu akūtūhalaṃ;",
  "anya-e1005n:278_0": "Piyañca annapānesu,",
  "anya-e1005n:279_0": "Atthe jāte ca paṇḍitaṃ.",
  "anya-e1005n:280_0": "55.",
  "anya-e1005n:281_0": "Sunakho sunakhaṃ disvā,",
  "anya-e1005n:282_0": "Dantaṃ dasseti hiṃsituṃ;",
  "anya-e1005n:283_0": "Dujjano sujanaṃ disvā,",
  "anya-e1005n:284_0": "Rosayaṃ hiṃsamicchati.",
  "anya-e1005n:285_0": "56.",
  "anya-e1005n:286_0": "Mā ca vegena kiccāni,",
  "anya-e1005n:287_0": "Karosi kārāpesi vā;",
  "anya-e1005n:288_0": "Sahasā kāritaṃ kammaṃ,",
  "anya-e1005n:289_0": "Mando pacchānutappati.",
  "anya-e1005n:290_0": "57.",
  "anya-e1005n:291_0": "Kodhaṃ vihitvā na kadācinasoce,",
  "anya-e1005n:292_0": "Makkhappahānaṃ isayo avaṇṇayuṃ;",
  "anya-e1005n:293_0": "Sabbesa phārusa-vacaṃ khametha,",
  "anya-e1005n:294_0": "Etaṃ khantiṃ uttamamāhu santo.",
  "anya-e1005n:295_0": "58.",
  "anya-e1005n:296_0": "Dukkho nivāso sambādhe,",
  "anya-e1005n:297_0": "Ṭhāne asucisaṅkate;",
  "anya-e1005n:298_0": "Tato arimhi appiye,",
  "anya-e1005n:299_0": "Tatopi akataññunā.",
  "anya-e1005n:300_0": "59.",
  "anya-e1005n:301_0": "Ovādeyyānusāseyya,",
  "anya-e1005n:302_0": "Gāpakā ca nivāraye;",
  "anya-e1005n:303_0": "Satañhi so piyo hoti,",
  "anya-e1005n:304_0": "Asataṃ hoti appiyo.",
  "anya-e1005n:305_0": "60.",
  "anya-e1005n:306_0": "Uttamattanivātena,",
  "anya-e1005n:307_0": "Sūraṃ bhedena nijjaye;",
  "anya-e1005n:308_0": "Nīcaṃ appaka dānena,",
  "anya-e1005n:309_0": "Vīriyena samaṃ jaye.",
  "anya-e1005n:310_0": "61.",
  "anya-e1005n:311_0": "Na visaṃ visamiccāhu,",
  "anya-e1005n:312_0": "Dhanaṃ saṅghassa uccate;",
  "anya-e1005n:313_0": "Visaṃ ekaṃva hanati,",
  "anya-e1005n:314_0": "Sabbaṃ saṅghassa santakaṃ.",
  "anya-e1005n:315_0": "62.",
  "anya-e1005n:316_0": "Javane bhadraṃ jānanti,",
  "anya-e1005n:317_0": "Baliddañca vāhenā;",
  "anya-e1005n:318_0": "Duhena dhenuṃ jānanti,",
  "anya-e1005n:319_0": "Bhāsamānena paṇḍitaṃ.",
  "anya-e1005n:320_0": "63.",
  "anya-e1005n:321_0": "Dhanamappampi sādhūnaṃ,",
  "anya-e1005n:322_0": "Kūpe vāriva nissayo;",
  "anya-e1005n:323_0": "Bahuṃ api asādhūnaṃ,",
  "anya-e1005n:324_0": "Naca vārīva aṇṇave.",
  "anya-e1005n:325_0": "64.",
  "anya-e1005n:326_0": "Najjo pivanti no āpaṃ,",
  "anya-e1005n:327_0": "Rukkhā khādanti no phalaṃ;",
  "anya-e1005n:328_0": "Vassanti kvaci no meghā,",
  "anya-e1005n:329_0": "Paratthāya sataṃ dhanaṃ.",
  "anya-e1005n:330_0": "65.",
  "anya-e1005n:331_0": "Apattheyyaṃ na pattheyya,",
  "anya-e1005n:332_0": "A cinteyyaṃ na cintaye;",
  "anya-e1005n:333_0": "Dhammameva sucinteyya,",
  "anya-e1005n:334_0": "Kālaṃ moghaṃ na accaye.",
  "anya-e1005n:335_0": "66.",
  "anya-e1005n:336_0": "Acintitampi bhavati,",
  "anya-e1005n:337_0": "Cintitampi vinassati;",
  "anya-e1005n:338_0": "Na hi cintāmayā bhogā,",
  "anya-e1005n:339_0": "Itthiyā purisassavā.",
  "anya-e1005n:340_0": "67.",
  "anya-e1005n:341_0": "Asantassa piyo hoti,",
  "anya-e1005n:342_0": "Sante na kurute piyaṃ;",
  "anya-e1005n:343_0": "Asataṃ dhammaṃ roceti,",
  "anya-e1005n:344_0": "Taṃ parābhavato mukhaṃ.",
  "anya-e1005n:345_0": "Sujanakaṇḍo niṭṭhito.",
  "anya-e1005n:346_0": "4. Bāladujjanakaṇḍo",
  "anya-e1005n:347_0": "68.",
  "anya-e1005n:348_0": "Atippiyo na kātabbo,",
  "anya-e1005n:349_0": "Khalo kotūhalaṃ karo;",
  "anya-e1005n:350_0": "Sirasā vahyamānopi,",
  "anya-e1005n:351_0": "Aḍḍhapūro ghaṭo yathā.",
  "anya-e1005n:352_0": "69.",
  "anya-e1005n:353_0": "Sappo duṭṭho khalo duṭṭho,",
  "anya-e1005n:354_0": "Sappo duṭṭhataro khalo;",
  "anya-e1005n:355_0": "Mantosadhehi so sappo,",
  "anya-e1005n:356_0": "Khalo kenupasammati.",
  "anya-e1005n:357_0": "70.",
  "anya-e1005n:358_0": "Yo bālo maññati bālyaṃ,",
  "anya-e1005n:359_0": "Paṇḍito vāpi tena so;",
  "anya-e1005n:360_0": "Bālova paṇḍitamānī,",
  "anya-e1005n:361_0": "So ve bāloti vuccati.",
  "anya-e1005n:362_0": "71.",
  "anya-e1005n:363_0": "Madhuṃva maññatī bālo,",
  "anya-e1005n:364_0": "Yāva pāpaṃ na paccati;",
  "anya-e1005n:365_0": "Yadāca paccatī pāpaṃ,",
  "anya-e1005n:366_0": "Atha dukkhaṃ nigacchati.",
  "anya-e1005n:367_0": "72.",
  "anya-e1005n:368_0": "Na sādhu balavā bālo,",
  "anya-e1005n:369_0": "Sahasā vindate dhanaṃ;",
  "anya-e1005n:370_0": "Kāyassa bhedā duppañño,",
  "anya-e1005n:371_0": "Nirayaṃ sopapajjati.",
  "anya-e1005n:372_0": "73.",
  "anya-e1005n:373_0": "Ghare duṭṭho ca mūsīko ca,",
  "anya-e1005n:374_0": "Vane duṭṭho ca vānaro;",
  "anya-e1005n:375_0": "Sakuṇe ca duṭṭho kāko,",
  "anya-e1005n:376_0": "Nare duṭṭhoca brāhmaṇo.",
  "anya-e1005n:377_0": "74.",
  "anya-e1005n:378_0": "Dīghā jāgarato ratti,",
  "anya-e1005n:379_0": "Dīghaṃ santassa yojanaṃ;",
  "anya-e1005n:380_0": "Dīgho bālāna saṃsāro,",
  "anya-e1005n:381_0": "Saddhammaṃ a-vijānataṃ.",
  "anya-e1005n:382_0": "75.",
  "anya-e1005n:383_0": "Tila mattaṃ paresaṃva,",
  "anya-e1005n:384_0": "Appa dosañca passati;",
  "anya-e1005n:385_0": "Nāḷikerampi sadosaṃ,",
  "anya-e1005n:386_0": "Khala-jāto na passati.",
  "anya-e1005n:387_0": "76.",
  "anya-e1005n:388_0": "Nattadosaṃ pare jaññā,",
  "anya-e1005n:389_0": "Jaññā dosaṃ parassatu;",
  "anya-e1005n:390_0": "Guyhe kummo aṅgāni,",
  "anya-e1005n:391_0": "Para dosañca lakkhaye.",
  "anya-e1005n:392_0": "77.",
  "anya-e1005n:393_0": "Paṇḍitassa pasaṃsāya,",
  "anya-e1005n:394_0": "Daṇḍo bālena dīyate;",
  "anya-e1005n:395_0": "Paṇḍito paṇḍiteneva,",
  "anya-e1005n:396_0": "Vaṇṇitova suvaṇṇito.",
  "anya-e1005n:397_0": "78.",
  "anya-e1005n:398_0": "Luddhaṃ atthena gaṇheyya,",
  "anya-e1005n:399_0": "Thaddhaṃ añjali kammunā;",
  "anya-e1005n:400_0": "Chandānuvattiyā mūḷhaṃ,",
  "anya-e1005n:401_0": "Yathābhūtena paṇḍitaṃ.",
  "anya-e1005n:402_0": "Bāladujjanakaṇḍo niṭṭhito.",
  "anya-e1005n:403_0": "4. Mittakaṇḍo",
  "anya-e1005n:404_0": "79.",
  "anya-e1005n:405_0": "Hitakāro paro bandhu,",
  "anya-e1005n:406_0": "Bandhūpi ahito paro;",
  "anya-e1005n:407_0": "A hito dehajo byādhi,",
  "anya-e1005n:408_0": "Hitaṃ araññamosadhaṃ.",
  "anya-e1005n:409_0": "80.",
  "anya-e1005n:410_0": "Parokkhe guṇa-hantāraṃ,",
  "anya-e1005n:411_0": "Paccakkhe piyavādinaṃ;",
  "anya-e1005n:412_0": "Vajjeyya tādisaṃ mittaṃ,",
  "anya-e1005n:413_0": "Visakumbhe yathā madhuṃ.",
  "anya-e1005n:414_0": "81.",
  "anya-e1005n:415_0": "Dhanahīne caje mitto,",
  "anya-e1005n:416_0": "Puttadārā sahodarā;",
  "anya-e1005n:417_0": "Dhanavantaṃva sevanti,",
  "anya-e1005n:418_0": "Dhanaṃ loke mahā sakhā.",
  "anya-e1005n:419_0": "82.",
  "anya-e1005n:420_0": "Jāneyya pesena bhaccaṃ,",
  "anya-e1005n:421_0": "Bandhuṃ vāpi bhayāgate;",
  "anya-e1005n:422_0": "Appakāsu tathā mittaṃ,",
  "anya-e1005n:423_0": "Dārañca vibhavakkhaye.",
  "anya-e1005n:424_0": "83.",
  "anya-e1005n:425_0": "So bandhu yo hite yutto,",
  "anya-e1005n:426_0": "Pitaro honti posako;",
  "anya-e1005n:427_0": "Taṃ mittaṃ yattha vissāso,",
  "anya-e1005n:428_0": "Sā bhariyā ca yassa nibbūti.",
  "anya-e1005n:429_0": "84.",
  "anya-e1005n:430_0": "Na vissase a-vissattaṃ,",
  "anya-e1005n:431_0": "Mittañcāpi na vissase;",
  "anya-e1005n:432_0": "Kadāci kupito mito,",
  "anya-e1005n:433_0": "Sabbaṃ dosaṃ pakāsaye.",
  "anya-e1005n:434_0": "85.",
  "anya-e1005n:435_0": "Sakiṃ duṭṭhañca yo mittaṃ,",
  "anya-e1005n:436_0": "Puna sandhitumicchati;",
  "anya-e1005n:437_0": "So maccuṃ upagaṇhāti,",
  "anya-e1005n:438_0": "Gabbhamassatarī yathā.",
  "anya-e1005n:439_0": "86.",
  "anya-e1005n:440_0": "Vahe a-mittaṃ khandhena,",
  "anya-e1005n:441_0": "Yāva kālo anāgato;",
  "anya-e1005n:442_0": "Tameva āgate kāle,",
  "anya-e1005n:443_0": "Sele bhinde ghaṭaṃ iva.",
  "anya-e1005n:444_0": "87.",
  "anya-e1005n:445_0": "Iṇaseso aggiseso,",
  "anya-e1005n:446_0": "Satruseso tatheva ca;",
  "anya-e1005n:447_0": "Punappunaṃ vivaḍḍhanti,",
  "anya-e1005n:448_0": "Tasmā sesaṃ na kāraye.",
  "anya-e1005n:449_0": "88.",
  "anya-e1005n:450_0": "Padumaṃva mukhaṃ yassa,",
  "anya-e1005n:451_0": "Vācā candana sītalā;",
  "anya-e1005n:452_0": "Tādisaṃ nopaseveyya,",
  "anya-e1005n:453_0": "Hadayetu halāhalaṃ.",
  "anya-e1005n:454_0": "89.",
  "anya-e1005n:455_0": "Na seve pharusaṃ sāmiṃ,",
  "anya-e1005n:456_0": "Naca seveyya macchariṃ;",
  "anya-e1005n:457_0": "Tato apaggaṇhaṃ sāmiṃ,",
  "anya-e1005n:458_0": "Neva niggahitaṃ tato.",
  "anya-e1005n:459_0": "90.",
  "anya-e1005n:460_0": "Siṅgī paññāsa hatthena,",
  "anya-e1005n:461_0": "Vajje satena vājinaṃ;",
  "anya-e1005n:462_0": "Hatthiṃ dantiṃ sahassena,",
  "anya-e1005n:463_0": "Desa cāgena dujjanaṃ.",
  "anya-e1005n:464_0": "91.",
  "anya-e1005n:465_0": "Kudesañca kumittañca,",
  "anya-e1005n:466_0": "Kukulañca kubandhavaṃ;",
  "anya-e1005n:467_0": "Kudārañca kudāsañca,",
  "anya-e1005n:468_0": "Dūrato parivajjaye.",
  "anya-e1005n:469_0": "92.",
  "anya-e1005n:470_0": "Rogāture ca dubbhikkhe,",
  "anya-e1005n:471_0": "Byasane sattu viggahe;",
  "anya-e1005n:472_0": "Rājadvāre susāne ca,",
  "anya-e1005n:473_0": "Ye tiṭṭhanti sumittakā.",
  "anya-e1005n:474_0": "93.",
  "anya-e1005n:475_0": "Sītavāco bahumitto,",
  "anya-e1005n:476_0": "Pharuso appamittako;",
  "anya-e1005n:477_0": "Upamaṃ ettha ñātabbā,",
  "anya-e1005n:478_0": "Canda-sūriya-rājūnaṃ.",
  "anya-e1005n:479_0": "Mittakaṇḍo niṭṭhito.",
  "anya-e1005n:480_0": "5. Itthikaṇḍo",
  "anya-e1005n:481_0": "94.",
  "anya-e1005n:482_0": "Kokilānaṃ saddaṃ rūpaṃ,",
  "anya-e1005n:483_0": "Nārīrūpaṃ patibbatā;",
  "anya-e1005n:484_0": "Vijjā rūpaṃ a-rūpānaṃ,",
  "anya-e1005n:485_0": "Khamā rūpaṃ tapassinaṃ.",
  "anya-e1005n:486_0": "95.",
  "anya-e1005n:487_0": "Itthīnañca dhanaṃ rūpaṃ,",
  "anya-e1005n:488_0": "Purisānaṃ vijjā dhanaṃ;",
  "anya-e1005n:489_0": "Bhikkhūnañca dhanaṃ sīlaṃ,",
  "anya-e1005n:490_0": "Rājānañca dhanaṃ balaṃ.",
  "anya-e1005n:491_0": "96.",
  "anya-e1005n:492_0": "Tapassino kisā sobhā,",
  "anya-e1005n:493_0": "Thūlā sobhā catuppadā;",
  "anya-e1005n:494_0": "Purisā vijjavā sobhā,",
  "anya-e1005n:495_0": "Itthī sobhāsa sāmikā.",
  "anya-e1005n:496_0": "97.",
  "anya-e1005n:497_0": "Pañca ratyā sugandhabbā,",
  "anya-e1005n:498_0": "Satta ratyā dhanuggahā;",
  "anya-e1005n:499_0": "Eka māsā subhariyā,",
  "anya-e1005n:500_0": "Aḍḍha māsā sissā malā.",
  "anya-e1005n:501_0": "98.",
  "anya-e1005n:502_0": "Hiṃ ramati paṅa,",
  "anya-e1005n:503_0": "Haṅa ramati poka.",
  "anya-e1005n:504_0": "Thī ramati pu,",
  "anya-e1005n:505_0": "Khu ramati dhaṃ.",
  "anya-e1005n:506_0": "99.",
  "anya-e1005n:507_0": "Jiṇṇamannaṃ pasaṃseyya,",
  "anya-e1005n:508_0": "Dārañca gatayobbanaṃ;",
  "anya-e1005n:509_0": "Raṇā punāgatā sūraṃ,",
  "anya-e1005n:510_0": "Sassañca gehamāgataṃ.",
  "anya-e1005n:511_0": "100.",
  "anya-e1005n:512_0": "Dvatti-patikā nārī ca,",
  "anya-e1005n:513_0": "Bhikkhu dvatti-vihāriko;",
  "anya-e1005n:514_0": "Dvatti-pāsa-mutto pakkhī,",
  "anya-e1005n:515_0": "Kata-māyā bahūtharaṃ.",
  "anya-e1005n:516_0": "101.",
  "anya-e1005n:517_0": "Dujjanaṃ pahārādame,",
  "anya-e1005n:518_0": "Mittaṃ dame a-bhāṇikā;",
  "anya-e1005n:519_0": "Itthiñca byasanā dame,",
  "anya-e1005n:520_0": "Rāginaṃ appa bhojanā.",
  "anya-e1005n:521_0": "102.",
  "anya-e1005n:522_0": "Na ratti vinā candimā,",
  "anya-e1005n:523_0": "Vīciṃ vinā ca aṇṇavo;",
  "anya-e1005n:524_0": "Haṃsaṃ vinā pokkharaṇī,",
  "anya-e1005n:525_0": "Patiṃ kaññāca sobhate.",
  "anya-e1005n:526_0": "103.",
  "anya-e1005n:527_0": "Patinā janito bhogo,",
  "anya-e1005n:528_0": "Itthiyāva saṃgoppito;",
  "anya-e1005n:529_0": "Purisova hi padhāno,",
  "anya-e1005n:530_0": "Itthī suttaṃva sūciyā.",
  "anya-e1005n:531_0": "104.",
  "anya-e1005n:532_0": "Sabbānadī vaṅkanadī,",
  "anya-e1005n:533_0": "Sabbe kaṭṭhamayā vanā;",
  "anya-e1005n:534_0": "Sabbitthiyo kare pāpaṃ,",
  "anya-e1005n:535_0": "Labhamāne nivātake.",
  "anya-e1005n:536_0": "105.",
  "anya-e1005n:537_0": "Vivādasīliṃ usūyabhāṇiniṃ,",
  "anya-e1005n:538_0": "Sampassataṇhiṃ bahupākabhuttiniṃ;",
  "anya-e1005n:539_0": "Aggantabhuttiṃ paragehavāsiniṃ,",
  "anya-e1005n:540_0": "Nāriṃ caje puttasatampi pūmā.",
  "anya-e1005n:541_0": "106.",
  "anya-e1005n:542_0": "Bhuttesu maṇḍesu janīva kantinī,",
  "anya-e1005n:543_0": "Guyheca ṭhāne bhaginīva hiriṇī;",
  "anya-e1005n:544_0": "Kammesu pattesu karoti dāsīva,",
  "anya-e1005n:545_0": "Bhayesu mantī sayanesu rāmaye;",
  "anya-e1005n:546_0": "Rūpīsu sikkhī kupanesu khantinī,",
  "anya-e1005n:547_0": "Sā nārī seṭṭhāti vadanti paṇḍitā;",
  "anya-e1005n:548_0": "Kāyassa bhedāca divebhaveyya sā.",
  "anya-e1005n:549_0": "107.",
  "anya-e1005n:550_0": "Sāmā migakkhī tanumajjhagattā,",
  "anya-e1005n:551_0": "Sūrū sukesī samadantapantī;",
  "anya-e1005n:552_0": "Gambhīranābhī yuvatī susīlī,",
  "anya-e1005n:553_0": "Hīne kule jātāpi vivāhyā.",
  "anya-e1005n:554_0": "108.",
  "anya-e1005n:555_0": "Saradaṃratu-kālānaṃ,",
  "anya-e1005n:556_0": "Bhariyānaṃ rūpavatī;",
  "anya-e1005n:557_0": "Jeṭṭho padhānaṃ puttānaṃ,",
  "anya-e1005n:558_0": "Disānaṃ uttarādisā.",
  "anya-e1005n:559_0": "109.",
  "anya-e1005n:560_0": "Yā icche puriso hotuṃ,",
  "anya-e1005n:561_0": "Jāti jāti punappunaṃ;",
  "anya-e1005n:562_0": "Sāmikaṃ apacāyeyya,",
  "anya-e1005n:563_0": "Indaṃva pāricārikā.",
  "anya-e1005n:564_0": "110.",
  "anya-e1005n:565_0": "Yo icche puriso hotuṃ,",
  "anya-e1005n:566_0": "Jāti jāti punappunaṃ;",
  "anya-e1005n:567_0": "Paradānaṃ vivajjeyya,",
  "anya-e1005n:568_0": "Dhotapādova kaddamaṃ.",
  "anya-e1005n:569_0": "111.",
  "anya-e1005n:570_0": "Atikkanta vayo poso,",
  "anya-e1005n:571_0": "Āneti timbaruttaniṃ;",
  "anya-e1005n:572_0": "Tassā issā asaddhāti,",
  "anya-e1005n:573_0": "Taṃ parābhavato mukhaṃ.",
  "anya-e1005n:574_0": "Itthikaṇḍo niṭṭhito.",
  "anya-e1005n:575_0": "6. Rājakaṇḍo",
  "anya-e1005n:576_0": "112.",
  "anya-e1005n:577_0": "Ekayāmaṃ saye rājā,",
  "anya-e1005n:578_0": "Dviyāmaññeva paṇḍito;",
  "anya-e1005n:579_0": "Gharāvāso tiyāmaṃva,",
  "anya-e1005n:580_0": "Catuyāmaṃ tu yācako.",
  "anya-e1005n:581_0": "113.",
  "anya-e1005n:582_0": "Dhanavā sutavā rājā,",
  "anya-e1005n:583_0": "Nadī vejjo cimepañca;",
  "anya-e1005n:584_0": "Yattha dese na vijjanti,",
  "anya-e1005n:585_0": "Na tattha divasaṃ vase.",
  "anya-e1005n:586_0": "114.",
  "anya-e1005n:587_0": "Yasmiṃ padese na māno,",
  "anya-e1005n:588_0": "Na pemaṃ naca bandhavā;",
  "anya-e1005n:589_0": "Naca vijjāgamo koci,",
  "anya-e1005n:590_0": "Na tattha divasaṃ vase.",
  "anya-e1005n:591_0": "115.",
  "anya-e1005n:592_0": "Aputtakaṃ gharaṃ suññaṃ,",
  "anya-e1005n:593_0": "Raṭṭhaṃ suññaṃ arājakaṃ;",
  "anya-e1005n:594_0": "A sippassa mukhaṃ suññaṃ,",
  "anya-e1005n:595_0": "Sabba suññaṃ daliddakā.",
  "anya-e1005n:596_0": "116.",
  "anya-e1005n:597_0": "Dhanamiccheyya vāṇijjo,",
  "anya-e1005n:598_0": "Vijjamicche bhajesutaṃ;",
  "anya-e1005n:599_0": "Puttamicche taruṇitthiṃ,",
  "anya-e1005n:600_0": "Rājāmaccaṃ vasaṃ game.",
  "anya-e1005n:601_0": "117.",
  "anya-e1005n:602_0": "Naṭṭhoyati asantuṭṭho,",
  "anya-e1005n:603_0": "Santuṭṭho ca mahīpati;",
  "anya-e1005n:604_0": "Lajjā ca gaṇikā naṭṭhā,",
  "anya-e1005n:605_0": "Nillajjā kuladhītikā.",
  "anya-e1005n:606_0": "118.",
  "anya-e1005n:607_0": "Pakkhīnaṃ balamākāso,",
  "anya-e1005n:608_0": "Macchānamudakaṃ balaṃ;",
  "anya-e1005n:609_0": "Dubbalassa balaṃ rājā,",
  "anya-e1005n:610_0": "Kumārānaṃ rudaṃ balaṃ.",
  "anya-e1005n:611_0": "119.",
  "anya-e1005n:612_0": "Khamā jāgariyuṭṭhānaṃ,",
  "anya-e1005n:613_0": "Saṃvibhāgo dayikkhaṇā;",
  "anya-e1005n:614_0": "Nāyakassa guṇā ete,",
  "anya-e1005n:615_0": "Icchitabbā sataṃ guṇā.",
  "anya-e1005n:616_0": "120.",
  "anya-e1005n:617_0": "Sakiṃ vadanti rājāno,",
  "anya-e1005n:618_0": "Sakiṃ samaṇabrāhmaṇā;",
  "anya-e1005n:619_0": "Sakiṃ sappurisā loke,",
  "anya-e1005n:620_0": "Esa dhammo sanantano.",
  "anya-e1005n:621_0": "121.",
  "anya-e1005n:622_0": "Alaso gihī kāmabhogī na sādhu,",
  "anya-e1005n:623_0": "Asaññato pabbajito na sādhu;",
  "anya-e1005n:624_0": "Rājā anisammakārī na sādhu,",
  "anya-e1005n:625_0": "Paṇḍito kodhano taṃpi na sādhu.",
  "anya-e1005n:626_0": "122.",
  "anya-e1005n:627_0": "Bahavo yattha nettāro,",
  "anya-e1005n:628_0": "Sabbe paṇḍitamānino;",
  "anya-e1005n:629_0": "Sabbe mahattamicchanti,",
  "anya-e1005n:630_0": "Tesaṃ kammaṃ vinassati.",
  "anya-e1005n:631_0": "123.",
  "anya-e1005n:632_0": "Āyaṃ khayaṃ sayaṃ jaññā,",
  "anya-e1005n:633_0": "Rājā sayaṃ katākataṃ;",
  "anya-e1005n:634_0": "Niggahe niggahetabbaṃ,",
  "anya-e1005n:635_0": "Paggahe paggahārahaṃ.",
  "anya-e1005n:636_0": "124.",
  "anya-e1005n:637_0": "Piṭṭhitokkaṃ niseveyya,",
  "anya-e1005n:638_0": "Kucchināva hutāsanaṃ;",
  "anya-e1005n:639_0": "Sāmikaṃ sabbabhāgena,",
  "anya-e1005n:640_0": "Paralokaṃ amohavā.",
  "anya-e1005n:641_0": "125.",
  "anya-e1005n:642_0": "Aggi āpo itthimūḷho,",
  "anya-e1005n:643_0": "Sappo rāja-kulānica;",
  "anya-e1005n:644_0": "Apayantena gantabbā,",
  "anya-e1005n:645_0": "Acceka-pāṇahārakā.",
  "anya-e1005n:646_0": "126.",
  "anya-e1005n:647_0": "Paduṭṭha-bhariya-saṃvāso,",
  "anya-e1005n:648_0": "Paduṭṭha citta dāsako;",
  "anya-e1005n:649_0": "Sa-sappe ca ghare vāso,",
  "anya-e1005n:650_0": "Maccu eva na saṃsayo.",
  "anya-e1005n:651_0": "127.",
  "anya-e1005n:652_0": "Mūḷha sisso padesena,",
  "anya-e1005n:653_0": "Kunārī bharaṇena ca;",
  "anya-e1005n:654_0": "Asatā sampayogena,",
  "anya-e1005n:655_0": "Paṇḍitoppavasīdati.",
  "anya-e1005n:656_0": "128.",
  "anya-e1005n:657_0": "Mātā puttakaraṃ pāpaṃ,",
  "anya-e1005n:658_0": "Sissapāpaṃ gurukatā;",
  "anya-e1005n:659_0": "Rājā raṭṭhakaraṃ pāpaṃ,",
  "anya-e1005n:660_0": "Rājapāpaṃ purohito.",
  "anya-e1005n:661_0": "129.",
  "anya-e1005n:662_0": "Akodhena jine kodhaṃ,",
  "anya-e1005n:663_0": "Asādhuṃ sādhunā jine;",
  "anya-e1005n:664_0": "Jine macchariṃ dānena,",
  "anya-e1005n:665_0": "Saccenālīkavādinaṃ;",
  "anya-e1005n:666_0": "130.",
  "anya-e1005n:667_0": "Adantaṃ damanaṃ dānaṃ,",
  "anya-e1005n:668_0": "Dānaṃ sabbattha sādhakaṃ;",
  "anya-e1005n:669_0": "Dānena piya vācāya,",
  "anya-e1005n:670_0": "Unnamanti namanti ca;",
  "anya-e1005n:671_0": "131.",
  "anya-e1005n:672_0": "Dānaṃ sinehabhesajjaṃ,",
  "anya-e1005n:673_0": "Maccheraṃ dosanosadhaṃ;",
  "anya-e1005n:674_0": "Dānaṃ yasassī bhesajjaṃ,",
  "anya-e1005n:675_0": "Maccheraṃ kapaṇosadhaṃ.",
  "anya-e1005n:676_0": "132.",
  "anya-e1005n:677_0": "Bahūnamappasārānaṃ,",
  "anya-e1005n:678_0": "Sāmaggiyā jayaṃ jaye;",
  "anya-e1005n:679_0": "Tiṇehi vattate yottaṃ,",
  "anya-e1005n:680_0": "Tena nāgopi bajjhate.",
  "anya-e1005n:681_0": "133.",
  "anya-e1005n:682_0": "Sahāyo asamatthopi,",
  "anya-e1005n:683_0": "Tejasā kiṃkarissati;",
  "anya-e1005n:684_0": "Nivāte jalito aggi,",
  "anya-e1005n:685_0": "Sayame vūpasampati.",
  "anya-e1005n:686_0": "134.",
  "anya-e1005n:687_0": "Na raññā samakaṃ bhuñje,",
  "anya-e1005n:688_0": "Kāmabhogaṃ kudācanaṃ;",
  "anya-e1005n:689_0": "Ākappaṃ rasa bhuttiṃvā,",
  "anya-e1005n:690_0": "Mālā gandha vilepanaṃ;",
  "anya-e1005n:691_0": "Vatthaṃ sabbaalaṅkāraṃ,",
  "anya-e1005n:692_0": "Na raññā sadisaṃ kare.",
  "anya-e1005n:693_0": "135.",
  "anya-e1005n:694_0": "Na me rājā sakhā hoti,",
  "anya-e1005n:695_0": "Na rājā hoti methuno;",
  "anya-e1005n:696_0": "Eso sāmiko mayhanti,",
  "anya-e1005n:697_0": "Citte niṭṭhaṃ suthāpaye.",
  "anya-e1005n:698_0": "136.",
  "anya-e1005n:699_0": "Nātidūre bhaje rañño,",
  "anya-e1005n:700_0": "Nāccāsannopavātake;",
  "anya-e1005n:701_0": "Ujuke nātininne ca,",
  "anya-e1005n:702_0": "Na bhaje uccamāsane.",
  "anya-e1005n:703_0": "Chadose vajje sevako,",
  "anya-e1005n:704_0": "Tiṭṭhe aggiṃva saṃyato.",
  "anya-e1005n:705_0": "137.",
  "anya-e1005n:706_0": "Guṇī sabbaññu tulyopi,",
  "anya-e1005n:707_0": "Nasobhati anissayo;",
  "anya-e1005n:708_0": "Anagghamopi maṇiseṭṭho,",
  "anya-e1005n:709_0": "Hemaṃ nissāya sobhati.",
  "anya-e1005n:710_0": "Rājakaṇḍo niṭṭhito.",
  "anya-e1005n:711_0": "7. Pakiṇṇakakaṇḍo",
  "anya-e1005n:712_0": "138.",
  "anya-e1005n:713_0": "Itthimisse kutosīlaṃ,",
  "anya-e1005n:714_0": "Maṃsa bhakkhe kutodayā;",
  "anya-e1005n:715_0": "Surā pāne kutosaccaṃ,",
  "anya-e1005n:716_0": "Mahālobhe kutohirī;",
  "anya-e1005n:717_0": "Mahātande kutosippaṃ,",
  "anya-e1005n:718_0": "Mahā kodhe kutodhanaṃ.",
  "anya-e1005n:719_0": "139.",
  "anya-e1005n:720_0": "Surā yogo vikālo ca,",
  "anya-e1005n:721_0": "Samajja caraṇālasaṃ;",
  "anya-e1005n:722_0": "Khiḍḍādhutto pāpamitto,",
  "anya-e1005n:723_0": "Bhoganāsamukhā ime.",
  "anya-e1005n:724_0": "140.",
  "anya-e1005n:725_0": "Divā nādikkhā vattabbaṃ,",
  "anya-e1005n:726_0": "Ratto nāvacanena ca;",
  "anya-e1005n:727_0": "Sañcareyya bhayā bhīto,",
  "anya-e1005n:728_0": "Vane vanacarī yathā.",
  "anya-e1005n:729_0": "141.",
  "anya-e1005n:730_0": "Jīvantāpi matāpañca,",
  "anya-e1005n:731_0": "Byāsena parikittitā;",
  "anya-e1005n:732_0": "Dukkhito byādhitomūḷho,",
  "anya-e1005n:733_0": "Iṇavā nityasevako.",
  "anya-e1005n:734_0": "142.",
  "anya-e1005n:735_0": "Anāgataṃ bhayaṃ disvā,",
  "anya-e1005n:736_0": "Dūrato parivajjaye;",
  "anya-e1005n:737_0": "Āgatañca bhayaṃ disvā,",
  "anya-e1005n:738_0": "A bhīto hoti paṇḍito.",
  "anya-e1005n:739_0": "143.",
  "anya-e1005n:740_0": "Niddāluko pamattoca,",
  "anya-e1005n:741_0": "Sukhatto rogavālaso;",
  "anya-e1005n:742_0": "Mahiccho kammārāmoca,",
  "anya-e1005n:743_0": "Satte te satthavajjitā.",
  "anya-e1005n:744_0": "144.",
  "anya-e1005n:745_0": "Duggataṃ gaccha he lābha,",
  "anya-e1005n:746_0": "Lābhī lābhena pūrati;",
  "anya-e1005n:747_0": "Thale pavassa pajjunna,",
  "anya-e1005n:748_0": "Sindhu āpena pūrati;",
  "anya-e1005n:749_0": "Natthidaṃ kammappadhānakaṃ.",
  "anya-e1005n:750_0": "145.",
  "anya-e1005n:751_0": "Na hi koci kate kicce,",
  "anya-e1005n:752_0": "Kattāraṃ samupekkhate;",
  "anya-e1005n:753_0": "Tasmā sabbāni kiccāni,",
  "anya-e1005n:754_0": "Sāva sesena kāraye.",
  "anya-e1005n:755_0": "146.",
  "anya-e1005n:756_0": "Tūlaṃ sallahukaṃ loke,",
  "anya-e1005n:757_0": "Tato cāpalla-jātiko;",
  "anya-e1005n:758_0": "Tato vuḍḍha manovādo,",
  "anya-e1005n:759_0": "Pamatto buddhasāsane.",
  "anya-e1005n:760_0": "147.",
  "anya-e1005n:761_0": "Pāsāṇachattaṃ garukaṃ,",
  "anya-e1005n:762_0": "Tato devānacikkhaṇaṃ;",
  "anya-e1005n:763_0": "Tato vuḍḍhānamovādo,",
  "anya-e1005n:764_0": "Tato buddhassa sāsanaṃ.",
  "anya-e1005n:765_0": "148.",
  "anya-e1005n:766_0": "Kāyassa dakkhiṇa hattho,",
  "anya-e1005n:767_0": "Doso ettha kaniṭṭhako;",
  "anya-e1005n:768_0": "Kaṇṇa ghānāna-makkhīnaṃ,",
  "anya-e1005n:769_0": "Vāmo tu pāda-pāsako.",
  "anya-e1005n:770_0": "149.",
  "anya-e1005n:771_0": "Tambūlassa majjha patte,",
  "anya-e1005n:772_0": "Kuvero rakkhatī sadā;",
  "anya-e1005n:773_0": "Mūlamhi rakkhati yakkho,",
  "anya-e1005n:774_0": "Aggamhi kālakaṇṇikā;",
  "anya-e1005n:775_0": "Tāni bhuñjeyya chinditvā,",
  "anya-e1005n:776_0": "Sirī evaṃ pavaḍḍhati.",
  "anya-e1005n:777_0": "150.",
  "anya-e1005n:778_0": "Sampuṇṇarakkho brahmāva,",
  "anya-e1005n:779_0": "Accurakkho ca bissaṇo;",
  "anya-e1005n:780_0": "Tasmā hi te pūjayantu,",
  "anya-e1005n:781_0": "Sadā mānenti taṃ naraṃ.",
  "anya-e1005n:782_0": "151.",
  "anya-e1005n:783_0": "Goṇā hi sabbagihīnaṃ,",
  "anya-e1005n:784_0": "Posakā bhogadāyakā;",
  "anya-e1005n:785_0": "Tasmā hi mātā pitūva,",
  "anya-e1005n:786_0": "Mānaye sakkareyya ca.",
  "anya-e1005n:787_0": "152.",
  "anya-e1005n:788_0": "Yeca khādanti gomaṃsaṃ,",
  "anya-e1005n:789_0": "Mātu maṃsaṃva khādare;",
  "anya-e1005n:790_0": "Matesu tesu gijjhānaṃ,",
  "anya-e1005n:791_0": "Dade sote ca vāhaye.",
  "anya-e1005n:792_0": "153.",
  "anya-e1005n:793_0": "Gurusiddho sippārambho,",
  "anya-e1005n:794_0": "Ravi sokrā ca majjhimo;",
  "anya-e1005n:795_0": "Na sippo buddhacandaro,",
  "anya-e1005n:796_0": "Sorī aṅgāca maraṇaṃ.",
  "anya-e1005n:797_0": "154.",
  "anya-e1005n:798_0": "Aṭṭhamiyaṃ guruṃ hanti,",
  "anya-e1005n:799_0": "Sissaṃ hanti catuddasiṃ;",
  "anya-e1005n:800_0": "Sippaṃ hanti dasa sippaṃ,",
  "anya-e1005n:801_0": "Mātāpitā ca puṇṇamiṃ.",
  "anya-e1005n:802_0": "155.",
  "anya-e1005n:803_0": "Nāḷikaṃ satta nabhuñje,",
  "anya-e1005n:804_0": "Na lābuṃ navamaṃ tathā;",
  "anya-e1005n:805_0": "Dvādasa prinnaṃtriminaṃ,",
  "anya-e1005n:806_0": "Bhuñje sippaṃ vinassati.",
  "anya-e1005n:807_0": "156.",
  "anya-e1005n:808_0": "Ekaṃ caje kulaatthaṃ,",
  "anya-e1005n:809_0": "Gāmassatthaṃ kuvaṃ caje;",
  "anya-e1005n:810_0": "Gāma caje janapadatthaṃ,",
  "anya-e1005n:811_0": "Attatthaṃ pathaviṃ caje.",
  "anya-e1005n:812_0": "157.",
  "anya-e1005n:813_0": "Desaṃ ossajja gacchanti,",
  "anya-e1005n:814_0": "Sīho sappuriso gajo;",
  "anya-e1005n:815_0": "Tattheva nidhanaṃ yanti,",
  "anya-e1005n:816_0": "Kāko kāpuriso migo.",
  "anya-e1005n:817_0": "158.",
  "anya-e1005n:818_0": "Yamhi padese na māno,",
  "anya-e1005n:819_0": "Na pemaṃ na ca bandhavā;",
  "anya-e1005n:820_0": "Na ca vijjāgāho koci,",
  "anya-e1005n:821_0": "Na tattha vasanaṃ kare.",
  "anya-e1005n:822_0": "159.",
  "anya-e1005n:823_0": "Caratyekena pādena,",
  "anya-e1005n:824_0": "Tiṭṭhatyekena paṇḍito;",
  "anya-e1005n:825_0": "A nisamma paraṃ ṭhānaṃ,",
  "anya-e1005n:826_0": "Na pubbamālayaṃ jahe.",
  "anya-e1005n:827_0": "160.",
  "anya-e1005n:828_0": "Dhana dhañña payogesu,",
  "anya-e1005n:829_0": "Tathā vijjāgamesu ca;",
  "anya-e1005n:830_0": "Dūtesu apacāresu,",
  "anya-e1005n:831_0": "Cajjā lajjā tadā bhave.",
  "anya-e1005n:832_0": "161.",
  "anya-e1005n:833_0": "Dvi guṇo thīnamāhāro,",
  "anya-e1005n:834_0": "Buddhicāpi catugguṇo;",
  "anya-e1005n:835_0": "Chagguṇo hoti vāyāmo,",
  "anya-e1005n:836_0": "Kāmotvaṭṭha-guṇo bhave.",
  "anya-e1005n:837_0": "162.",
  "anya-e1005n:838_0": "Pabbe pabbe kamenucchu,",
  "anya-e1005n:839_0": "Visesarasavāggato;",
  "anya-e1005n:840_0": "Tathā sumettiko sādhu,",
  "anya-e1005n:841_0": "Viparītova dujjano.",
  "anya-e1005n:842_0": "163.",
  "anya-e1005n:843_0": "Kassako vāṇijo macco,",
  "anya-e1005n:844_0": "Samaṇo suta sīlavā;",
  "anya-e1005n:845_0": "Tesu vipula jātesu,",
  "anya-e1005n:846_0": "Raṭṭhampi vipulaṃ siyā.",
  "anya-e1005n:847_0": "164.",
  "anya-e1005n:848_0": "Asajjhāya malā mantā,",
  "anya-e1005n:849_0": "Anuṭṭhāna malā gharā;",
  "anya-e1005n:850_0": "Malaṃ vaṇṇassa kosajjaṃ,",
  "anya-e1005n:851_0": "Pamādo rakkhato malaṃ.",
  "anya-e1005n:852_0": "165.",
  "anya-e1005n:853_0": "Hīnānaṃ gacchate vittaṃ,",
  "anya-e1005n:854_0": "Vīrānaṃ santakattanaṃ;",
  "anya-e1005n:855_0": "Vadanti ca hīnā janā,",
  "anya-e1005n:856_0": "Pubba-kammappadhānakā.",
  "anya-e1005n:857_0": "166.",
  "anya-e1005n:858_0": "Na vadanti cevaṃdhīrā,",
  "anya-e1005n:859_0": "Vāyamiṃsu sabbakamme;",
  "anya-e1005n:860_0": "Na ce sijjhati taṃ kammaṃ,",
  "anya-e1005n:861_0": "A-phalaṃ eva ko doso.",
  "anya-e1005n:862_0": "167.",
  "anya-e1005n:863_0": "Nīcaṃ kulaṃ nipaññaṃ vā,",
  "anya-e1005n:864_0": "Nirūpaṃ nibalaṃ samaṃ;",
  "anya-e1005n:865_0": "Imaṃ kālaṃ chuttakālaṃ,",
  "anya-e1005n:866_0": "Dhanameva visesakaṃ.",
  "anya-e1005n:867_0": "Pakiṇṇakakaṇḍo niṭṭhito.",
  "anya-e1005n:868_0": "Paṇḍito sujano kaṇḍo,",
  "anya-e1005n:869_0": "Dujjano mitta-itthi ca;",
  "anya-e1005n:870_0": "Rājā pakiṇṇako cāti,",
  "anya-e1005n:871_0": "Satta-kaṇḍa-vibhūsitaṃ.",
  "anya-e1005n:872_0": "Visuddhā cāra-therena,",
  "anya-e1005n:873_0": "Visuddhārāma-vāsinā;",
  "anya-e1005n:874_0": "Sabba-kulānamatthāya,",
  "anya-e1005n:875_0": "Visodhitaṃ pathakkhaye."
}