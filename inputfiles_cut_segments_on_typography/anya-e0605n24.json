{
  "anya-e0605n24:0_0": "Dhammacakkapavattanadīpanīgāthā",
  "anya-e0605n24:1_0": "174.",
  "anya-e0605n24:2_0": "Sammāsambodhiñānaṁ hatasakalamalaṁ suddhato cātisuddhaṁ,",
  "anya-e0605n24:3_0": "Addhā laddhā suladdhaṁ vatamiti satataṁ cintayanto subodhiṁ;",
  "anya-e0605n24:4_0": "Sattāhaṁ sattamevaṁ vividhaphalasukhaṁ vitināmesi kālaṁ,",
  "anya-e0605n24:5_0": "Brahmenāyācito so isipatanavane vattayī dhammacakkaṁ."
}