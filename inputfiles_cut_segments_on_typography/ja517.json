{
  "ja517:0.1_0": "Jātaka",
  "ja517:0.2_0": "Tiṁsanipāta",
  "ja517:0.3_0": "Kiṁchandavagga",
  "ja517:0.4_0": "7. Dakarakkhasajātaka",
  "ja517:1.1_0": "“Sace vo vuyhamānānaṁ,",
  "ja517:1.2_0": "sattannaṁ udakaṇṇave;",
  "ja517:1.3_0": "Manussabalimesāno,",
  "ja517:1.4_0": "nāvaṁ gaṇheyya rakkhaso;",
  "ja517:1.5_0": "Anupubbaṁ kathaṁ datvā,",
  "ja517:1.6_0": "muñcesi dakarakkhasā”.",
  "ja517:2.1_0": "“Mātaraṁ paṭhamaṁ dajjaṁ,",
  "ja517:2.2_0": "Bhariyaṁ datvāna bhātaraṁ;",
  "ja517:2.3_0": "Tato sahāyaṁ datvāna,",
  "ja517:2.4_0": "Pañcamaṁ dajjaṁ brāhmaṇaṁ;",
  "ja517:2.5_0": "Chaṭṭhāhaṁ dajjamattānaṁ,",
  "ja517:2.6_0": "Neva dajjaṁ mahosadhaṁ”.",
  "ja517:3.1_0": "“Posetā te janettī ca,",
  "ja517:3.2_0": "dīgharattānukampikā;",
  "ja517:3.3_0": "Chabbhī tayi padussati,",
  "ja517:3.4_0": "paṇḍitā atthadassinī;",
  "ja517:3.5_0": "Aññaṁ upanisaṁ katvā,",
  "ja517:3.6_0": "vadhā taṁ parimocayi.",
  "ja517:4.1_0": "Taṁ tādisiṁ pāṇadadiṁ,",
  "ja517:4.2_0": "orasaṁ gabbhadhāriniṁ;",
  "ja517:4.3_0": "Mātaraṁ kena dosena,",
  "ja517:4.4_0": "dajjāsi dakarakkhino”.",
  "ja517:5.1_0": "“Daharā viyalaṅkāraṁ,",
  "ja517:5.2_0": "dhāreti apiḷandhanaṁ;",
  "ja517:5.3_0": "Dovārike anīkaṭṭhe,",
  "ja517:5.4_0": "ativelaṁ pajagghati.",
  "ja517:6.1_0": "Athopi paṭirājūnaṁ,",
  "ja517:6.2_0": "sayaṁ dūtāni sāsati;",
  "ja517:6.3_0": "Mātaraṁ tena dosena,",
  "ja517:6.4_0": "dajjāhaṁ dakarakkhino”.",
  "ja517:7.1_0": "“Itthigumbassa pavarā,",
  "ja517:7.2_0": "accantaṁ piyabhāṇinī;",
  "ja517:7.3_0": "Anuggatā sīlavatī,",
  "ja517:7.4_0": "chāyāva anapāyinī.",
  "ja517:8.1_0": "Akkodhanā puññavatī,",
  "ja517:8.2_0": "paṇḍitā atthadassinī;",
  "ja517:8.3_0": "Ubbariṁ kena dosena,",
  "ja517:8.4_0": "dajjāsi dakarakkhino”.",
  "ja517:9.1_0": "“Khiḍḍāratisamāpannaṁ,",
  "ja517:9.2_0": "anatthavasamāgataṁ;",
  "ja517:9.3_0": "Sā maṁ sakāna puttānaṁ,",
  "ja517:9.4_0": "ayācaṁ yācate dhanaṁ.",
  "ja517:10.1_0": "Sohaṁ dadāmi sāratto,",
  "ja517:10.2_0": "bahuṁ uccāvacaṁ dhanaṁ;",
  "ja517:10.3_0": "Suduccajaṁ cajitvāna,",
  "ja517:10.4_0": "pacchā socāmi dummano;",
  "ja517:10.5_0": "Ubbariṁ tena dosena,",
  "ja517:10.6_0": "dajjāhaṁ dakarakkhino”.",
  "ja517:11.1_0": "“Yenocitā janapadā,",
  "ja517:11.2_0": "ānītā ca paṭiggahaṁ;",
  "ja517:11.3_0": "Ābhataṁ pararajjebhi,",
  "ja517:11.4_0": "abhiṭṭhāya bahuṁ dhanaṁ.",
  "ja517:12.1_0": "Dhanuggahānaṁ pavaraṁ,",
  "ja517:12.2_0": "sūraṁ tikhiṇamantinaṁ;",
  "ja517:12.3_0": "Bhātaraṁ kena dosena,",
  "ja517:12.4_0": "dajjāsi dakarakkhino”.",
  "ja517:13.1_0": "“Yenocitā janapadā,",
  "ja517:13.2_0": "ānītā ca paṭiggahaṁ;",
  "ja517:13.3_0": "Ābhataṁ pararajjebhi,",
  "ja517:13.4_0": "abhiṭṭhāya bahuṁ dhanaṁ.",
  "ja517:14.1_0": "Dhanuggahānaṁ pavaro,",
  "ja517:14.2_0": "sūro tikhiṇamanti ca;",
  "ja517:14.3_0": "Mayāyaṁ sukhito rājā,",
  "ja517:14.4_0": "atimaññati dārako.",
  "ja517:15.1_0": "Upaṭṭhānampi me ayye,",
  "ja517:15.2_0": "na so eti yathā pure;",
  "ja517:15.3_0": "Bhātaraṁ tena dosena,",
  "ja517:15.4_0": "dajjāhaṁ dakarakkhino”.",
  "ja517:16.1_0": "“Ekarattena ubhayo,",
  "ja517:16.2_0": "tvañceva dhanusekha ca;",
  "ja517:16.3_0": "Ubho jātettha pañcālā,",
  "ja517:16.4_0": "sahāyā susamāvayā.",
  "ja517:17.1_0": "Cariyā taṁ anubandhittho,",
  "ja517:17.2_0": "ekadukkhasukho tava;",
  "ja517:17.3_0": "Ussukko te divārattiṁ,",
  "ja517:17.4_0": "sabbakiccesu byāvaṭo;",
  "ja517:17.5_0": "Sahāyaṁ kena dosena,",
  "ja517:17.6_0": "dajjāsi dakarakkhino”.",
  "ja517:18.1_0": "“Cariyā maṁ ayaṁ ayye,",
  "ja517:18.2_0": "pajagghittho mayā saha;",
  "ja517:18.3_0": "Ajjāpi tena vaṇṇena,",
  "ja517:18.4_0": "ativelaṁ pajagghati.",
  "ja517:19.1_0": "Ubbariyāpihaṁ ayye,",
  "ja517:19.2_0": "mantayāmi rahogato;",
  "ja517:19.3_0": "Anāmanto pavisati,",
  "ja517:19.4_0": "pubbe appaṭivedito.",
  "ja517:20.1_0": "Laddhadvāro katokāso,",
  "ja517:20.2_0": "ahirikaṁ anādaraṁ;",
  "ja517:20.3_0": "Sahāyaṁ tena dosena,",
  "ja517:20.4_0": "dajjāhaṁ dakarakkhino”.",
  "ja517:21.1_0": "“Kusalo sabbanimittānaṁ,",
  "ja517:21.2_0": "rutaññū āgatāgamo;",
  "ja517:21.3_0": "Uppāte supine yutto,",
  "ja517:21.4_0": "niyyāne ca pavesane.",
  "ja517:22.1_0": "Paṭṭho bhūmantalikkhasmiṁ,",
  "ja517:22.2_0": "Nakkhattapadakovido;",
  "ja517:22.3_0": "Brāhmaṇaṁ kena dosena,",
  "ja517:22.4_0": "Dajjāsi dakarakkhino”.",
  "ja517:23.1_0": "“Parisāyampi me ayye,",
  "ja517:23.2_0": "ummīlitvā udikkhati;",
  "ja517:23.3_0": "Tasmā accabhamuṁ luddaṁ,",
  "ja517:23.4_0": "dajjāhaṁ dakarakkhino”.",
  "ja517:24.1_0": "“Sasamuddapariyāyaṁ,",
  "ja517:24.2_0": "mahiṁ sāgarakuṇḍalaṁ;",
  "ja517:24.3_0": "Vasundharaṁ āvasasi,",
  "ja517:24.4_0": "amaccaparivārito.",
  "ja517:25.1_0": "Cāturanto mahāraṭṭho,",
  "ja517:25.2_0": "vijitāvī mahabbalo;",
  "ja517:25.3_0": "Pathabyā ekarājāsi,",
  "ja517:25.4_0": "yaso te vipulaṁ gato.",
  "ja517:26.1_0": "Soḷasitthisahassāni,",
  "ja517:26.2_0": "āmuttamaṇikuṇḍalā;",
  "ja517:26.3_0": "Nānājanapadā nārī,",
  "ja517:26.4_0": "devakaññūpamā subhā.",
  "ja517:27.1_0": "Evaṁ sabbaṅgasampannaṁ,",
  "ja517:27.2_0": "sabbakāmasamiddhinaṁ;",
  "ja517:27.3_0": "Sukhitānaṁ piyaṁ dīghaṁ,",
  "ja517:27.4_0": "jīvitaṁ āhu khattiya.",
  "ja517:28.1_0": "Atha tvaṁ kena vaṇṇena,",
  "ja517:28.2_0": "kena vā pana hetunā;",
  "ja517:28.3_0": "Paṇḍitaṁ anurakkhanto,",
  "ja517:28.4_0": "pāṇaṁ cajasi duccajaṁ”.",
  "ja517:29.1_0": "“Yatopi āgato ayye,",
  "ja517:29.2_0": "mama hatthaṁ mahosadho;",
  "ja517:29.3_0": "Nābhijānāmi dhīrassa,",
  "ja517:29.4_0": "anumattampi dukkaṭaṁ.",
  "ja517:30.1_0": "Sace ca kismici kāle,",
  "ja517:30.2_0": "maraṇaṁ me pure siyā;",
  "ja517:30.3_0": "So me putte paputte ca,",
  "ja517:30.4_0": "sukhāpeyya mahosadho.",
  "ja517:31.1_0": "Anāgataṁ paccuppannaṁ,",
  "ja517:31.2_0": "sabbamatthampi passati;",
  "ja517:31.3_0": "Anāparādhakammantaṁ,",
  "ja517:31.4_0": "na dajjaṁ dakarakkhino”.",
  "ja517:32.1_0": "“Idaṁ suṇātha pañcālā,",
  "ja517:32.2_0": "cūḷaneyyassa bhāsitaṁ;",
  "ja517:32.3_0": "Paṇḍitaṁ anurakkhanto,",
  "ja517:32.4_0": "pāṇaṁ cajati duccajaṁ.",
  "ja517:33.1_0": "Mātu bhariyāya bhātucca,",
  "ja517:33.2_0": "sakhino brāhmaṇassa ca;",
  "ja517:33.3_0": "Attano cāpi pañcālo,",
  "ja517:33.4_0": "channaṁ cajati jīvitaṁ.",
  "ja517:34.1_0": "Evaṁ mahatthikā paññā,",
  "ja517:34.2_0": "nipuṇā sādhucintinī;",
  "ja517:34.3_0": "Diṭṭhadhammahitatthāya,",
  "ja517:34.4_0": "samparāyasukhāya cā”ti.",
  "ja517:35.1_0": "Dakarakkhasajātakaṁ sattamaṁ."
}