{
  "ja493:0.1_0": "Jātaka",
  "ja493:0.2_0": "Pakiṇṇakanipāta",
  "ja493:0.3_0": "Sālikedāravagga",
  "ja493:0.4_0": "10. Mahāvāṇijajātaka",
  "ja493:1.1_0": "Vāṇijā samitiṁ katvā,",
  "ja493:1.2_0": "nānāraṭṭhato āgatā;",
  "ja493:1.3_0": "Dhanāharā pakkamiṁsu,",
  "ja493:1.4_0": "ekaṁ katvāna gāmaṇiṁ.",
  "ja493:2.1_0": "Te taṁ kantāramāgamma,",
  "ja493:2.2_0": "appabhakkhaṁ anodakaṁ;",
  "ja493:2.3_0": "Mahānigrodhamaddakkhuṁ,",
  "ja493:2.4_0": "sītacchāyaṁ manoramaṁ.",
  "ja493:3.1_0": "Te ca tattha nisīditvā,",
  "ja493:3.2_0": "tassa rukkhassa chāyayā;",
  "ja493:3.3_0": "Vāṇijā samacintesuṁ,",
  "ja493:3.4_0": "bālā mohena pārutā.",
  "ja493:4.1_0": "Allāyate ayaṁ rukkho,",
  "ja493:4.2_0": "api vārīva sandati;",
  "ja493:4.3_0": "Iṅghassa purimaṁ sākhaṁ,",
  "ja493:4.4_0": "mayaṁ chindāma vāṇijā.",
  "ja493:5.1_0": "Sā ca chinnāva pagghari,",
  "ja493:5.2_0": "acchaṁ vāriṁ anāvilaṁ;",
  "ja493:5.3_0": "Te tattha nhatvā pivitvā,",
  "ja493:5.4_0": "yāvaticchiṁsu vāṇijā.",
  "ja493:6.1_0": "Dutiyaṁ samacintesuṁ,",
  "ja493:6.2_0": "bālā mohena pārutā;",
  "ja493:6.3_0": "Iṅghassa dakkhiṇaṁ sākhaṁ,",
  "ja493:6.4_0": "mayaṁ chindāma vāṇijā.",
  "ja493:7.1_0": "Sā ca chinnāva pagghari,",
  "ja493:7.2_0": "sālimaṁsodanaṁ bahuṁ;",
  "ja493:7.3_0": "Appodavaṇṇe kummāse,",
  "ja493:7.4_0": "siṅgiṁ vidalasūpiyo.",
  "ja493:8.1_0": "Te tattha bhutvā khāditvā,",
  "ja493:8.2_0": "yāvaticchiṁsu vāṇijā;",
  "ja493:8.3_0": "Tatiyaṁ samacintesuṁ,",
  "ja493:8.4_0": "bālā mohena pārutā;",
  "ja493:8.5_0": "Iṅghassa pacchimaṁ sākhaṁ,",
  "ja493:8.6_0": "mayaṁ chindāma vāṇijā.",
  "ja493:9.1_0": "Sā ca chinnāva pagghari,",
  "ja493:9.2_0": "nāriyo samalaṅkatā;",
  "ja493:9.3_0": "Vicitravatthābharaṇā,",
  "ja493:9.4_0": "āmuttamaṇikuṇḍalā.",
  "ja493:10.1_0": "Api su vāṇijā ekā,",
  "ja493:10.2_0": "nāriyo paṇṇavīsati;",
  "ja493:10.3_0": "Samantā parivāriṁsu,",
  "ja493:10.4_0": "tassa rukkhassa chāyayā.",
  "ja493:11.1_0": "Te tāhi paricāretvā,",
  "ja493:11.2_0": "yāvaticchiṁsu vāṇijā;",
  "ja493:11.3_0": "Catutthaṁ samacintesuṁ,",
  "ja493:11.4_0": "bālā mohena pārutā;",
  "ja493:11.5_0": "Iṅghassa uttaraṁ sākhaṁ,",
  "ja493:11.6_0": "mayaṁ chindāma vāṇijā.",
  "ja493:12.1_0": "Sā ca chinnāva pagghari,",
  "ja493:12.2_0": "muttā veḷuriyā bahū;",
  "ja493:12.3_0": "Rajataṁ jātarūpañca,",
  "ja493:12.4_0": "kuttiyo paṭiyāni ca.",
  "ja493:13.1_0": "Kāsikāni ca vatthāni,",
  "ja493:13.2_0": "uddiyāni ca kambalā;",
  "ja493:13.3_0": "Te tattha bhāre bandhitvā,",
  "ja493:13.4_0": "yāvaticchiṁsu vāṇijā.",
  "ja493:14.1_0": "Pañcamaṁ samacintesuṁ,",
  "ja493:14.2_0": "bālā mohena pārutā;",
  "ja493:14.3_0": "Iṅghassa mūlaṁ chindāma,",
  "ja493:14.4_0": "api bhiyyo labhāmase.",
  "ja493:15.1_0": "Athuṭṭhahi satthavāho,",
  "ja493:15.2_0": "yācamāno katañjalī;",
  "ja493:15.3_0": "Nigrodho kiṁ parajjhati,",
  "ja493:15.4_0": "vāṇijā bhaddamatthu te.",
  "ja493:16.1_0": "Vāridā purimā sākhā,",
  "ja493:16.2_0": "annapānañca dakkhiṇā;",
  "ja493:16.3_0": "Nāridā pacchimā sākhā,",
  "ja493:16.4_0": "sabbakāme ca uttarā;",
  "ja493:16.5_0": "Nigrodho kiṁ parajjhati,",
  "ja493:16.6_0": "vāṇijā bhaddamatthu te.",
  "ja493:17.1_0": "Yassa rukkhassa chāyāya,",
  "ja493:17.2_0": "nisīdeyya sayeyya vā;",
  "ja493:17.3_0": "Na tassa sākhaṁ bhañjeyya,",
  "ja493:17.4_0": "mittadubbho hi pāpako.",
  "ja493:18.1_0": "Te ca tassānādiyitvā,",
  "ja493:18.2_0": "ekassa vacanaṁ bahū;",
  "ja493:18.3_0": "Nisitāhi kuṭhārīhi,",
  "ja493:18.4_0": "mūlato naṁ upakkamuṁ.",
  "ja493:19.1_0": "Tato nāgā nikkhamiṁsu,",
  "ja493:19.2_0": "sannaddhā paṇṇavīsati;",
  "ja493:19.3_0": "Dhanuggahānaṁ tisatā,",
  "ja493:19.4_0": "chasahassā ca vammino.",
  "ja493:20.1_0": "“Ete hanatha bandhatha,",
  "ja493:20.2_0": "Mā vo muñcittha jīvitaṁ;",
  "ja493:20.3_0": "Ṭhapetvā satthavāhaṁva,",
  "ja493:20.4_0": "Sabbe bhasmaṁ karotha ne”.",
  "ja493:21.1_0": "“Tasmā hi paṇḍito poso,",
  "ja493:21.2_0": "sampassaṁ atthamattano;",
  "ja493:21.3_0": "Lobhassa na vasaṁ gacche,",
  "ja493:21.4_0": "haneyyārisakaṁ manaṁ.",
  "ja493:22.1_0": "Evamādīnavaṁ ñatvā,",
  "ja493:22.2_0": "taṇhā dukkhassa sambhavaṁ;",
  "ja493:22.3_0": "Vītataṇho anādāno,",
  "ja493:22.4_0": "sato bhikkhu paribbaje”ti.",
  "ja493:23.1_0": "Mahāvāṇijajātakaṁ dasamaṁ."
}