{
  "thag1.60:0.1_0": "Theragāthā 1.60",
  "thag1.60:0.2_0": "Ekakanipāta",
  "thag1.60:0.3_0": "Chaṭṭhavagga",
  "thag1.60:0.4_0": "Sīvalittheragāthā",
  "thag1.60:1.1_0": "“Te me ijjhiṁsu saṅkappā,",
  "thag1.60:1.2_0": "yadattho pāvisiṁ kuṭiṁ;",
  "thag1.60:1.3_0": "Vijjāvimuttiṁ paccesaṁ,",
  "thag1.60:1.4_0": "mānānusayamujjahan”ti.",
  "thag1.60:1.5_0": "… Sīvalitthero …",
  "thag1.60:1.6_0": "vaggo chaṭṭho.",
  "thag1.60:2.0_0": "Tassuddānaṁ",
  "thag1.60:2.1_0": "Godhiko ca subāhu ca,",
  "thag1.60:2.2_0": "valliyo uttiyo isi;",
  "thag1.60:2.3_0": "Añjanavaniyo thero,",
  "thag1.60:2.4_0": "duve kuṭivihārino;",
  "thag1.60:2.5_0": "Ramaṇīyakuṭiko ca,",
  "thag1.60:2.6_0": "kosalavhayasīvalīti."
}