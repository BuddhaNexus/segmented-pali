{
  "kv13.2:0.1_0": "Kathāvatthu",
  "kv13.2:0.2_0": "Mahāpaṇṇāsaka",
  "kv13.2:0.3_0": "Terasamavagga",
  "kv13.2:0.4_0": "Kusalapaṭilābhakathā",
  "kv13.2:1.1_0": "Kappaṭṭho kusalaṁ cittaṁ na paṭilabheyyāti?",
  "kv13.2:1.2_0": "Āmantā.",
  "kv13.2:1.3_0": "Kappaṭṭho dānaṁ dadeyyāti?",
  "kv13.2:1.4_0": "Āmantā.",
  "kv13.2:1.5_0": "Hañci kappaṭṭho dānaṁ dadeyya, no ca vata re vattabbe—",
  "kv13.2:1.6_0": "“kappaṭṭho kusalaṁ cittaṁ na paṭilabheyyā”ti.",
  "kv13.2:2.1_0": "Kappaṭṭho kusalaṁ cittaṁ na paṭilabheyyāti?",
  "kv13.2:2.2_0": "Āmantā.",
  "kv13.2:2.3_0": "Kappaṭṭho cīvaraṁ dadeyya …pe…",
  "kv13.2:2.4_0": "piṇḍapātaṁ dadeyya …pe…",
  "kv13.2:2.5_0": "senāsanaṁ dadeyya …pe…",
  "kv13.2:2.6_0": "gilānapaccayabhesajjaparikkhāraṁ dadeyya …",
  "kv13.2:2.7_0": "khādanīyaṁ dadeyya …",
  "kv13.2:2.8_0": "bhojanīyaṁ dadeyya …",
  "kv13.2:2.9_0": "pānīyaṁ dadeyya …",
  "kv13.2:2.10_0": "cetiyaṁ vandeyya …",
  "kv13.2:2.11_0": "cetiye mālaṁ āropeyya …",
  "kv13.2:2.12_0": "gandhaṁ āropeyya …",
  "kv13.2:2.13_0": "vilepanaṁ āropeyya …pe…",
  "kv13.2:2.14_0": "cetiyaṁ abhidakkhiṇaṁ kareyyāti?",
  "kv13.2:2.15_0": "Āmantā.",
  "kv13.2:2.16_0": "Hañci kappaṭṭho cetiyaṁ abhidakkhiṇaṁ kareyya, no ca vata re vattabbe—",
  "kv13.2:2.17_0": "“kappaṭṭho kusalaṁ cittaṁ na paṭilabheyyā”ti …pe….",
  "kv13.2:3.1_0": "Kappaṭṭho kusalaṁ cittaṁ paṭilabheyyāti?",
  "kv13.2:3.2_0": "Āmantā.",
  "kv13.2:3.3_0": "Tato vuṭṭhānaṁ kusalaṁ cittaṁ paṭilabheyyāti?",
  "kv13.2:3.4_0": "Āmantā.",
  "kv13.2:3.5_0": "Rūpāvacaraṁ …pe…",
  "kv13.2:3.6_0": "arūpāvacaraṁ …pe…",
  "kv13.2:3.7_0": "lokuttaraṁ kusalaṁ cittaṁ paṭilabheyyāti?",
  "kv13.2:3.8_0": "Na hevaṁ vattabbe …pe….",
  "kv13.2:3.9_0": "Kusalapaṭilābhakathā niṭṭhitā."
}