{
  "tha-ap510:0.1_0": "Therāpadāna",
  "tha-ap510:0.2_0": "Kaṇikāravagga",
  "tha-ap510:0.3_0": "8 Amodaphaliyattheraapadāna",
  "tha-ap510:1.1_0": "“Suvaṇṇavaṇṇaṁ sambuddhaṁ,",
  "tha-ap510:1.2_0": "Āhutīnaṁ paṭiggahaṁ;",
  "tha-ap510:1.3_0": "Rathiyaṁ paṭipajjantaṁ,",
  "tha-ap510:1.4_0": "Āmodamadadiṁ phalaṁ.",
  "tha-ap510:2.1_0": "Ekanavutito kappe,",
  "tha-ap510:2.2_0": "yaṁ phalaṁ adadiṁ tadā;",
  "tha-ap510:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap510:2.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap510:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap510:3.2_0": "…pe…",
  "tha-ap510:3.3_0": "viharāmi anāsavo.",
  "tha-ap510:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap510:4.2_0": "…pe…",
  "tha-ap510:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap510:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap510:5.2_0": "…pe…",
  "tha-ap510:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap510:6.1_0": "Itthaṁ sudaṁ āyasmā amodaphaliyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap510:7.1_0": "Amodaphaliyattherassāpadānaṁ aṭṭhamaṁ."
}