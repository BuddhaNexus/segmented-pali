{
  "kv14.9:0.1_0": "Kathāvatthu",
  "kv14.9:0.2_0": "Mahāpaṇṇāsaka",
  "kv14.9:0.3_0": "Cuddasamavagga",
  "kv14.9:0.4_0": "Apariyāpannakathā",
  "kv14.9:1.1_0": "Diṭṭhigataṁ apariyāpannanti?",
  "kv14.9:1.2_0": "Āmantā.",
  "kv14.9:1.3_0": "Maggo phalaṁ nibbānaṁ,",
  "kv14.9:1.3_1": "sotāpattimaggo sotāpattiphalaṁ,",
  "kv14.9:1.3_2": "sakadāgāmimaggo sakadāgāmiphalaṁ,",
  "kv14.9:1.3_3": "anāgāmimaggo anāgāmiphalaṁ,",
  "kv14.9:1.3_4": "arahattamaggo arahattaphalaṁ,",
  "kv14.9:1.3_5": "satipaṭṭhānaṁ sammappadhānaṁ iddhipādo indriyaṁ balaṁ bojjhaṅgoti?",
  "kv14.9:1.4_0": "Na hevaṁ vattabbe …pe….",
  "kv14.9:2.1_0": "Na vattabbaṁ—",
  "kv14.9:2.2_0": "“diṭṭhigataṁ apariyāpannan”ti?",
  "kv14.9:2.3_0": "Āmantā.",
  "kv14.9:2.4_0": "Puthujjano “kāmesu vītarāgo”ti vattabboti?",
  "kv14.9:2.5_0": "Āmantā.",
  "kv14.9:2.6_0": "“Vigatadiṭṭhiyo”ti vattabboti?",
  "kv14.9:2.7_0": "Na hevaṁ vattabbe.",
  "kv14.9:2.8_0": "Tena hi diṭṭhigataṁ apariyāpannanti.",
  "kv14.9:2.9_0": "Apariyāpannakathā niṭṭhitā.",
  "kv14.9:2.10_0": "Cuddasamo vaggo.",
  "kv14.9:2.11_0": "Tassuddānaṁ",
  "kv14.9:3.1_0": "Akusalamūlaṁ paṭisandahati kusalamūlaṁ,",
  "kv14.9:3.2_0": "kusalamūlaṁ paṭisandahati akusalamūlaṁ,",
  "kv14.9:3.3_0": "saḷāyatanaṁ chaviññāṇakāyā,",
  "kv14.9:3.4_0": "ariyarūpaṁ mahābhūtānaṁ upādāya,",
  "kv14.9:3.5_0": "sveva anusayo taṁ pariyuṭṭhānaṁ,",
  "kv14.9:3.6_0": "pariyuṭṭhānaṁ cittavippayuttaṁ,",
  "kv14.9:3.7_0": "yathādhātu taññeva anuseti,",
  "kv14.9:3.8_0": "diṭṭhigataṁ abyākataṁ,",
  "kv14.9:3.9_0": "diṭṭhigataṁ apariyāpannanti."
}