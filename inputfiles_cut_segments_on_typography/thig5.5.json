{
  "thig5.5:0.1_0": "Therīgāthā 5.5",
  "thig5.5:0.2_0": "Pañcakanipāta",
  "thig5.5:0.3_0": "Nanduttarātherīgāthā",
  "thig5.5:1.1_0": "“Aggiṁ candañca sūriyañca,",
  "thig5.5:1.2_0": "devatā ca namassihaṁ;",
  "thig5.5:1.3_0": "Nadītitthāni gantvāna,",
  "thig5.5:1.4_0": "udakaṁ oruhāmihaṁ.",
  "thig5.5:2.1_0": "Bahūvatasamādānā,",
  "thig5.5:2.2_0": "aḍḍhaṁ sīsassa olikhiṁ;",
  "thig5.5:2.3_0": "Chamāya seyyaṁ kappemi,",
  "thig5.5:2.4_0": "rattiṁ bhattaṁ na bhuñjahaṁ.",
  "thig5.5:3.1_0": "Vibhūsāmaṇḍanaratā,",
  "thig5.5:3.2_0": "nhāpanucchādanehi ca;",
  "thig5.5:3.3_0": "Upakāsiṁ imaṁ kāyaṁ,",
  "thig5.5:3.4_0": "kāmarāgena aṭṭitā.",
  "thig5.5:4.1_0": "Tato saddhaṁ labhitvāna,",
  "thig5.5:4.2_0": "pabbajiṁ anagāriyaṁ;",
  "thig5.5:4.3_0": "Disvā kāyaṁ yathābhūtaṁ,",
  "thig5.5:4.4_0": "kāmarāgo samūhato.",
  "thig5.5:5.1_0": "Sabbe bhavā samucchinnā,",
  "thig5.5:5.2_0": "icchā ca patthanāpi ca;",
  "thig5.5:5.3_0": "Sabbayogavisaṁyuttā,",
  "thig5.5:5.4_0": "santiṁ pāpuṇi cetaso”ti.",
  "thig5.5:6.1_0": "… Nanduttarā therī …."
}