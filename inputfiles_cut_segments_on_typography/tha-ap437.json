{
  "tha-ap437:0.1_0": "Therāpadāna",
  "tha-ap437:0.2_0": "Ekavihārivagga",
  "tha-ap437:0.3_0": "5. Ucchukhaṇḍikattheraapadāna",
  "tha-ap437:1.1_0": "“Nagare bandhumatiyā,",
  "tha-ap437:1.2_0": "dvārapālo ahosahaṁ;",
  "tha-ap437:1.3_0": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap437:1.4_0": "sabbadhammāna pāraguṁ.",
  "tha-ap437:2.1_0": "Ucchukhaṇḍikamādāya,",
  "tha-ap437:2.2_0": "buddhaseṭṭhassadāsahaṁ;",
  "tha-ap437:2.3_0": "Pasannacitto sumano,",
  "tha-ap437:2.4_0": "vipassissa mahesino.",
  "tha-ap437:3.1_0": "Ekanavutito kappe,",
  "tha-ap437:3.2_0": "yaṁ ucchumadadiṁ tadā;",
  "tha-ap437:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap437:3.4_0": "ucchukhaṇḍassidaṁ phalaṁ.",
  "tha-ap437:4.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap437:4.2_0": "…pe…",
  "tha-ap437:4.3_0": "viharāmi anāsavo.",
  "tha-ap437:5.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap437:5.2_0": "…pe…",
  "tha-ap437:5.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap437:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap437:6.2_0": "…pe…",
  "tha-ap437:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap437:7.1_0": "Itthaṁ sudaṁ āyasmā ucchukhaṇḍiko thero imā gāthāyo abhāsitthāti.",
  "tha-ap437:8.1_0": "Ucchukhaṇḍikattherassāpadānaṁ pañcamaṁ."
}