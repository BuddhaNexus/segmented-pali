{
  "vv7:0.1_0": "Vimānavatthu",
  "vv7:0.2_0": "Itthivimāna",
  "vv7:0.3_0": "Pīṭhavagga",
  "vv7:0.4_0": "7. Dutiyanāvāvimānavatthu",
  "vv7:1.1_0": "“Suvaṇṇacchadanaṁ nāvaṁ,",
  "vv7:1.2_0": "Nāri āruyha tiṭṭhasi;",
  "vv7:1.3_0": "Ogāhasi pokkharaṇiṁ,",
  "vv7:1.4_0": "Padmaṁ chindasi pāṇinā.",
  "vv7:2.1_0": "Kena tetādiso vaṇṇo,",
  "vv7:2.2_0": "kena te idha mijjhati;",
  "vv7:2.3_0": "Uppajjanti ca te bhogā,",
  "vv7:2.4_0": "ye keci manaso piyā.",
  "vv7:3.1_0": "Pucchāmi taṁ devi mahānubhāve,",
  "vv7:3.2_0": "Manussabhūtā kimakāsi puññaṁ;",
  "vv7:3.3_0": "Kenāsi evaṁ jalitānubhāvā,",
  "vv7:3.4_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv7:4.1_0": "Sā devatā attamanā,",
  "vv7:4.2_0": "moggallānena pucchitā;",
  "vv7:4.3_0": "Pañhaṁ puṭṭhā viyākāsi,",
  "vv7:4.4_0": "yassa kammassidaṁ phalaṁ.",
  "vv7:5.1_0": "“Ahaṁ manussesu manussabhūtā,",
  "vv7:5.2_0": "Purimāya jātiyā manussaloke;",
  "vv7:5.3_0": "Disvāna bhikkhuṁ tasitaṁ kilantaṁ,",
  "vv7:5.4_0": "Uṭṭhāya pātuṁ udakaṁ adāsiṁ.",
  "vv7:6.1_0": "Yo ve kilantassa pipāsitassa,",
  "vv7:6.2_0": "Uṭṭhāya pātuṁ udakaṁ dadāti;",
  "vv7:6.3_0": "Sītodakā tassa bhavanti najjo,",
  "vv7:6.4_0": "Pahūtamalyā bahupuṇḍarīkā.",
  "vv7:7.1_0": "Taṁ āpagā anupariyanti sabbadā,",
  "vv7:7.2_0": "Sītodakā vālukasanthatā nadī;",
  "vv7:7.3_0": "Ambā ca sālā tilakā ca jambuyo,",
  "vv7:7.4_0": "Uddālakā pāṭaliyo ca phullā.",
  "vv7:8.1_0": "Taṁ bhūmibhāgehi upetarūpaṁ,",
  "vv7:8.2_0": "Vimānaseṭṭhaṁ bhusa sobhamānaṁ;",
  "vv7:8.3_0": "Tassīdha kammassa ayaṁ vipāko,",
  "vv7:8.4_0": "Etādisaṁ puññakatā labhanti.",
  "vv7:9.1_0": "Tena metādiso vaṇṇo,",
  "vv7:9.2_0": "tena me idha mijjhati;",
  "vv7:9.3_0": "Uppajjanti ca me bhogā,",
  "vv7:9.4_0": "ye keci manaso piyā.",
  "vv7:10.1_0": "Akkhāmi te bhikkhu mahānubhāva,",
  "vv7:10.2_0": "Manussabhūtā yamakāsi puññaṁ;",
  "vv7:10.3_0": "Tenamhi evaṁ jalitānubhāvā,",
  "vv7:10.4_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv7:11.1_0": "Dutiyanāvāvimānaṁ sattamaṁ."
}