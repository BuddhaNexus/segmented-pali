{
  "tha-ap312:0.1_0": "Therāpadāna",
  "tha-ap312:0.2_0": "Padumakesaravagga",
  "tha-ap312:0.3_0": "10. Pabbhāradāyakattheraapadāna",
  "tha-ap312:1.1_0": "“Piyadassino bhagavato,",
  "tha-ap312:1.2_0": "pabbhāro sodhito mayā;",
  "tha-ap312:1.3_0": "Ghaṭakañca upaṭṭhāsiṁ,",
  "tha-ap312:1.4_0": "paribhogāya tādino.",
  "tha-ap312:2.1_0": "Taṁ me buddho viyākāsi,",
  "tha-ap312:2.2_0": "piyadassī mahāmuni;",
  "tha-ap312:2.3_0": "Sahassakaṇḍo satabheṇḍu,",
  "tha-ap312:2.4_0": "dhajālu haritāmayo.",
  "tha-ap312:3.1_0": "Nibbattissati so yūpo,",
  "tha-ap312:3.2_0": "ratanañca anappakaṁ;",
  "tha-ap312:3.3_0": "Pabbhāradānaṁ datvāna,",
  "tha-ap312:3.4_0": "kappaṁ saggamhi modahaṁ.",
  "tha-ap312:4.1_0": "Ito bāttiṁsakappamhi,",
  "tha-ap312:4.2_0": "susuddho nāma khattiyo;",
  "tha-ap312:4.3_0": "Sattaratanasampanno,",
  "tha-ap312:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap312:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap312:5.2_0": "…pe…",
  "tha-ap312:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap312:6.1_0": "Itthaṁ sudaṁ āyasmā pabbhāradāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap312:7.1_0": "Pabbhāradāyakattherassāpadānaṁ dasamaṁ.",
  "tha-ap312:8.1_0": "Padumakesaravaggo ekatiṁsatimo.",
  "tha-ap312:9.1_0": "Tassuddānaṁ",
  "tha-ap312:10.1_0": "Kesaraṁ gandhamannañca,",
  "tha-ap312:10.2_0": "dhammasaññī phalena ca;",
  "tha-ap312:10.3_0": "Pasādārāmadāyī ca,",
  "tha-ap312:10.4_0": "lepako buddhasaññako;",
  "tha-ap312:10.5_0": "Pabbhārado ca gāthāyo,",
  "tha-ap312:10.6_0": "ekapaññāsa kittitā."
}