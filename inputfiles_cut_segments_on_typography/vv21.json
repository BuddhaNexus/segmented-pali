{
  "vv21:0.1_0": "Vimānavatthu",
  "vv21:0.2_0": "Itthivimāna",
  "vv21:0.3_0": "Cittalatāvagga",
  "vv21:0.4_0": "4. Caṇḍālivimānavatthu",
  "vv21:1.1_0": "“Caṇḍāli vanda pādāni,",
  "vv21:1.2_0": "gotamassa yasassino;",
  "vv21:1.3_0": "Tameva anukampāya,",
  "vv21:1.4_0": "aṭṭhāsi isisattamo.",
  "vv21:2.1_0": "Abhippasādehi manaṁ,",
  "vv21:2.2_0": "arahantamhi tādini;",
  "vv21:2.3_0": "Khippaṁ pañjalikā vanda,",
  "vv21:2.4_0": "parittaṁ tava jīvitan”ti.",
  "vv21:3.1_0": "Coditā bhāvitattena,",
  "vv21:3.2_0": "sarīrantimadhārinā;",
  "vv21:3.3_0": "Caṇḍālī vandi pādāni,",
  "vv21:3.4_0": "gotamassa yasassino.",
  "vv21:4.1_0": "Tamenaṁ avadhī gāvī,",
  "vv21:4.2_0": "caṇḍāliṁ pañjaliṁ ṭhitaṁ;",
  "vv21:4.3_0": "Namassamānaṁ sambuddhaṁ,",
  "vv21:4.4_0": "andhakāre pabhaṅkaranti.",
  "vv21:5.1_0": "“Khīṇāsavaṁ vigatarajaṁ anejaṁ,",
  "vv21:5.2_0": "Ekaṁ araññamhi raho nisinnaṁ;",
  "vv21:5.3_0": "Deviddhipattā upasaṅkamitvā,",
  "vv21:5.4_0": "Vandāmi taṁ vīra mahānubhāvan”ti.",
  "vv21:6.1_0": "“Suvaṇṇavaṇṇā jalitā mahāyasā,",
  "vv21:6.2_0": "Vimānamoruyha anekacittā;",
  "vv21:6.3_0": "Parivāritā accharāsaṅgaṇena,",
  "vv21:6.4_0": "Kā tvaṁ subhe devate vandase maman”ti.",
  "vv21:7.1_0": "“Ahaṁ bhaddante caṇḍālī,",
  "vv21:7.2_0": "tayā vīrena pesitā;",
  "vv21:7.3_0": "Vandiṁ arahato pāde,",
  "vv21:7.4_0": "gotamassa yasassino.",
  "vv21:8.1_0": "Sāhaṁ vanditvā pādāni,",
  "vv21:8.2_0": "cutā caṇḍālayoniyā;",
  "vv21:8.3_0": "Vimānaṁ sabbato bhaddaṁ,",
  "vv21:8.4_0": "upapannamhi nandane.",
  "vv21:9.1_0": "Accharānaṁ satasahassaṁ,",
  "vv21:9.2_0": "purakkhatvāna tiṭṭhati;",
  "vv21:9.3_0": "Tāsāhaṁ pavarā seṭṭhā,",
  "vv21:9.4_0": "vaṇṇena yasasāyunā.",
  "vv21:10.1_0": "Pahūtakatakalyāṇā,",
  "vv21:10.2_0": "Sampajānā paṭissatā;",
  "vv21:10.3_0": "Muniṁ kāruṇikaṁ loke,",
  "vv21:10.4_0": "Taṁ bhante vanditumāgatā”ti.",
  "vv21:11.1_0": "Idaṁ vatvāna caṇḍālī,",
  "vv21:11.2_0": "kataññū katavedinī;",
  "vv21:11.3_0": "Vanditvā arahato pāde,",
  "vv21:11.4_0": "tatthevantaradhāyathāti.",
  "vv21:12.1_0": "Caṇḍālivimānaṁ catutthaṁ."
}