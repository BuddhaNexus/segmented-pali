{
  "tha-ap142:0.1_0": "Therāpadāna",
  "tha-ap142:0.2_0": "Sobhitavagga",
  "tha-ap142:0.3_0": "10 Piyālaphaladāyakattheraapadāna",
  "tha-ap142:1.1_0": "“Pārāvato tadā āsiṁ,",
  "tha-ap142:1.2_0": "paraṁ anuparodhako;",
  "tha-ap142:1.3_0": "Pabbhāre seyyaṁ kappemi,",
  "tha-ap142:1.4_0": "avidūre sikhisatthuno.",
  "tha-ap142:2.1_0": "Sāyaṁ pātañca passāmi,",
  "tha-ap142:2.2_0": "buddhaṁ lokagganāyakaṁ;",
  "tha-ap142:2.3_0": "Deyyadhammo ca me natthi,",
  "tha-ap142:2.4_0": "dvipadindassa tādino.",
  "tha-ap142:3.1_0": "Piyālaphalamādāya,",
  "tha-ap142:3.2_0": "agamaṁ buddhasantikaṁ;",
  "tha-ap142:3.3_0": "Paṭiggahesi bhagavā,",
  "tha-ap142:3.4_0": "lokajeṭṭho narāsabho.",
  "tha-ap142:4.1_0": "Tato paraṁ upādāya,",
  "tha-ap142:4.2_0": "paricāriṁ vināyakaṁ;",
  "tha-ap142:4.3_0": "Tena cittappasādena,",
  "tha-ap142:4.4_0": "tattha kālaṅkato ahaṁ.",
  "tha-ap142:5.1_0": "Ekattiṁse ito kappe,",
  "tha-ap142:5.2_0": "yaṁ phalaṁ adadiṁ ahaṁ;",
  "tha-ap142:5.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap142:5.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap142:6.1_0": "Ito pannarase kappe,",
  "tha-ap142:6.2_0": "tayo āsuṁ piyālino;",
  "tha-ap142:6.3_0": "Sattaratanasampannā,",
  "tha-ap142:6.4_0": "cakkavattī mahabbalā.",
  "tha-ap142:7.1_0": "Paṭisambhidā catasso,",
  "tha-ap142:7.2_0": "…pe…",
  "tha-ap142:7.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap142:8.1_0": "Itthaṁ sudaṁ āyasmā piyālaphaladāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap142:9.1_0": "Piyālaphaladāyakattherassāpadānaṁ dasamaṁ.",
  "tha-ap142:10.1_0": "Sobhitavaggo cuddasamo.",
  "tha-ap142:11.1_0": "Tassuddānaṁ",
  "tha-ap142:12.1_0": "Sobhitasudassano ca,",
  "tha-ap142:12.2_0": "candano pupphachadano;",
  "tha-ap142:12.3_0": "Raho campakapupphī ca,",
  "tha-ap142:12.4_0": "atthasandassakena ca.",
  "tha-ap142:13.1_0": "Ekapasādī sāladado,",
  "tha-ap142:13.2_0": "dasamo phaladāyako;",
  "tha-ap142:13.3_0": "Gāthāyo sattati dve ca,",
  "tha-ap142:13.4_0": "gaṇitāyo vibhāvibhi."
}