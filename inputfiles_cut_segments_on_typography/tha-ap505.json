{
  "tha-ap505:0.1_0": "Therāpadāna",
  "tha-ap505:0.2_0": "Kaṇikāravagga",
  "tha-ap505:0.3_0": "3 Kāsumāraphaliyattheraapadāna",
  "tha-ap505:1.1_0": "“Kaṇikāraṁva jotantaṁ,",
  "tha-ap505:1.2_0": "nisinnaṁ pabbatantare;",
  "tha-ap505:1.3_0": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap505:1.4_0": "lokajeṭṭhaṁ narāsabhaṁ.",
  "tha-ap505:2.1_0": "Pasannacitto sumano,",
  "tha-ap505:2.2_0": "sire katvāna añjaliṁ;",
  "tha-ap505:2.3_0": "Kāsumārikamādāya,",
  "tha-ap505:2.4_0": "buddhaseṭṭhassadāsahaṁ.",
  "tha-ap505:3.1_0": "Ekatiṁse ito kappe,",
  "tha-ap505:3.2_0": "yaṁ phalaṁ adadiṁ tadā;",
  "tha-ap505:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap505:3.4_0": "phaladānassidaṁ phalaṁ.",
  "tha-ap505:4.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap505:4.2_0": "…pe…",
  "tha-ap505:4.3_0": "viharāmi anāsavo.",
  "tha-ap505:5.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap505:5.2_0": "…pe…",
  "tha-ap505:5.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap505:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap505:6.2_0": "…pe…",
  "tha-ap505:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap505:7.1_0": "Itthaṁ sudaṁ āyasmā kāsumāraphaliyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap505:8.1_0": "Kāsumāraphaliyattherassāpadānaṁ tatiyaṁ."
}