{
  "atk-s0402a47:0_0": "(12) 2. Kesivaggo",
  "atk-s0402a47:1_0": "1. Kesisuttavaṇṇanā",
  "atk-s0402a47:2_0": "111.",
  "atk-s0402a47:2_1": "Dutiyassa paṭhame kesīti tassa nāmaṁ.",
  "atk-s0402a47:2_2": "Assadamme sāretīti assadammasārathi.",
  "atk-s0402a47:2_3": "Saṇhenapi vinetītiādīsu tassa anucchavikaṁ sakkāraṁ katvā subhojanaṁ bhojetvā madhurapānaṁ pāyetvā muduvacanena",
  "atk-s0402a47:2_4": "samudācaritvā damento saṇhena dameti nāma,",
  "atk-s0402a47:2_5": "jāṇubandhanamukhabandhanādīhi ceva patodavijjhanakasābhighātapharusavacanehi ca damento pharusena dameti nāma,",
  "atk-s0402a47:2_6": "kālena kālaṁ tadubhayaṁ karonto saṇhapharusena dameti nāma.",
  "atk-s0402a47:3_0": "2. Javasuttavaṇṇanā",
  "atk-s0402a47:4_0": "112.",
  "atk-s0402a47:4_1": "Dutiye ajjavenāti ujukabhāvena.",
  "atk-s0402a47:4_2": "Javenāti padavegena.",
  "atk-s0402a47:4_3": "Khantiyāti adhivāsanakkhantiyā.",
  "atk-s0402a47:4_4": "Soraccenāti sucibhāvasīlena.",
  "atk-s0402a47:4_5": "Puggalaguṇaṅgesu javenāti ñāṇajavena.",
  "atk-s0402a47:4_6": "Sesamettha uttānatthameva.",
  "atk-s0402a47:5_0": "3. Patodasuttavaṇṇanā",
  "atk-s0402a47:6_0": "113.",
  "atk-s0402a47:6_1": "Tatiye patodacchāyanti vijjhanatthaṁ ukkhittassa patodassa chāyaṁ.",
  "atk-s0402a47:6_2": "Saṁvijjatīti “javo me gahetabbo”ti sallakkhaṇavasena saṁvijjati.",
  "atk-s0402a47:6_3": "Saṁvegaṁ āpajjatīti saṁvegaṁ paṭipajjati lomavedhaviddhoti lomakūpe patodavedhena viddhamatto.",
  "atk-s0402a47:6_4": "Cammavedhaviddhoti chavicammaṁ chindantena patodavedhena viddho.",
  "atk-s0402a47:6_5": "Aṭṭhivedhaviddhoti aṭṭhiṁ bhindantena vedhena viddho.",
  "atk-s0402a47:6_6": "Kāyenāti nāmakāyena.",
  "atk-s0402a47:6_7": "Paramasaccanti nibbānaṁ.",
  "atk-s0402a47:6_8": "Sacchikarotīti passati.",
  "atk-s0402a47:6_9": "Paññāyāti sahavipassanāya maggapaññāya.",
  "atk-s0402a47:7_0": "4. Nāgasuttavaṇṇanā",
  "atk-s0402a47:8_0": "114.",
  "atk-s0402a47:8_1": "Catutthe aṭṭhiṁ katvāti aṭṭhiko hutvā.",
  "atk-s0402a47:8_2": "Tiṇavaninnādasaddānanti ettha tiṇavoti ḍiṇḍimo,",
  "atk-s0402a47:8_3": "ninnādasaddoti sabbesampi ekatomissito mahāsaddo.",
  "atk-s0402a47:8_4": "Ḍaṁsādīsu ḍaṁsāti piṅgalamakkhikā,",
  "atk-s0402a47:8_5": "makasā makasāva.",
  "atk-s0402a47:8_6": "Khippaññeva gantā hotīti sīlasamādhipaññāvimuttivimuttiñāṇadassanāni pūretvā sīghameva gantā hoti.",
  "atk-s0402a47:9_0": "5. Ṭhānasuttavaṇṇanā",
  "atk-s0402a47:10_0": "115.",
  "atk-s0402a47:10_1": "Pañcame ṭhānānīti kāraṇāni.",
  "atk-s0402a47:10_2": "Anatthāya saṁvattatīti ahitāya avaḍḍhiyā saṁvattati.",
  "atk-s0402a47:10_3": "Ettha ca paṭhamaṁ opātakkhaṇanamacchabandhanasandhicchedanādibhedaṁ sadukkhaṁ savighātaṁ pāpakammaṁ veditabbaṁ,",
  "atk-s0402a47:10_4": "dutiyaṁ samajīvikānaṁ gihīnaṁ pupphacchaḍḍakādikammaṁ sudhākoṭṭana-",
  "atk-s0402a47:10_5": "gehacchādanaasuciṭṭhānasammajjanādikammañca veditabbaṁ,",
  "atk-s0402a47:10_6": "tatiyaṁ surāpānagandhavilepanamālāpiḷandhanādikammañceva assādavasena pavattaṁ pāṇātipātādikammañca veditabbaṁ,",
  "atk-s0402a47:10_7": "catutthaṁ dhammassavanatthāya gamanakāle suddhavatthacchādana-",
  "atk-s0402a47:10_8": "mālāgandhādīnaṁ ādāya gamanaṁ cetiyavandanaṁ bodhivandanaṁ madhuradhammakathāsavanaṁ pañcasīlasamādānanti",
  "atk-s0402a47:10_9": "evamādīsu somanassasampayuttaṁ kusalakammaṁ veditabbaṁ.",
  "atk-s0402a47:10_10": "Purisathāmeti purisassa ñāṇathāmasmiṁ.",
  "atk-s0402a47:10_11": "Sesadvayepi eseva nayo.",
  "atk-s0402a47:11_0": "6. Appamādasuttavaṇṇanā",
  "atk-s0402a47:12_0": "116.",
  "atk-s0402a47:12_1": "Chaṭṭhe yato khoti yadā kho.",
  "atk-s0402a47:12_2": "Samparāyikassāti desanāmattametaṁ,",
  "atk-s0402a47:12_3": "khīṇāsavo pana neva samparāyikassa,",
  "atk-s0402a47:12_4": "na diṭṭhadhammikassa maraṇassa bhāyati.",
  "atk-s0402a47:12_5": "Sova idha adhippeto.",
  "atk-s0402a47:12_6": "Keci pana “sammādiṭṭhi bhāvitāti vacanato sotāpannaṁ ādiṁ katvā sabbepi ariyā adhippetā”ti vadanti.",
  "atk-s0402a47:13_0": "7. Ārakkhasuttavaṇṇanā",
  "atk-s0402a47:14_0": "117.",
  "atk-s0402a47:14_1": "Sattame attarūpenāti attano anurūpena anucchavikena,",
  "atk-s0402a47:14_2": "hitakāmenāti attho.",
  "atk-s0402a47:14_3": "Rajanīyesūti rāgassa paccayabhūtesu.",
  "atk-s0402a47:14_4": "Dhammesūti sabhāvesu,",
  "atk-s0402a47:14_5": "iṭṭhārammaṇesūti attho.",
  "atk-s0402a47:14_6": "Evaṁ sabbattha nayo veditabbo.",
  "atk-s0402a47:14_7": "Na rajjatīti diṭṭhivasena na rajjati.",
  "atk-s0402a47:14_8": "Sesapadesupi eseva nayo.",
  "atk-s0402a47:14_9": "Na ca pana samaṇavacanahetupi gacchatīti samaṇānaṁ paravādīnaṁ vacanahetupi attano diṭṭhiṁ pahāya tesaṁ diṭṭhivasena na gacchatīti attho.",
  "atk-s0402a47:14_10": "Idhāpi khīṇāsavova adhippeto.",
  "atk-s0402a47:15_0": "8-10. Saṁvejanīyādisuttattayavaṇṇanā",
  "atk-s0402a47:16_0": "118- 120.",
  "atk-s0402a47:16_1": "Aṭṭhame dassanīyānīti passitabbayuttakāni.",
  "atk-s0402a47:16_2": "Saṁvejanīyānīti saṁvegajanakāni.",
  "atk-s0402a47:16_3": "Navame jātibhayanti jātiṁ ārabbha uppajjanakabhayaṁ.",
  "atk-s0402a47:16_4": "Sesapadesupi eseva nayo.",
  "atk-s0402a47:16_5": "Dasame aggibhayanti aggiṁ paṭicca uppajjanakabhayaṁ.",
  "atk-s0402a47:16_6": "Sesapadesupi eseva nayo.",
  "atk-s0402a47:17_0": "Kesivaggo dutiyo."
}