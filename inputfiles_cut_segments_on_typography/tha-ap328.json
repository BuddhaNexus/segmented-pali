{
  "tha-ap328:0.1_0": "Therāpadāna",
  "tha-ap328:0.2_0": "Umāpupphiyavagga",
  "tha-ap328:0.3_0": "6. Annasaṁsāvakattheraapadāna",
  "tha-ap328:1.1_0": "“Suvaṇṇavaṇṇaṁ sambuddhaṁ,",
  "tha-ap328:1.2_0": "Gacchantaṁ antarāpaṇe;",
  "tha-ap328:1.3_0": "Kañcanagghiyasaṅkāsaṁ,",
  "tha-ap328:1.4_0": "Bāttiṁsavaralakkhaṇaṁ.",
  "tha-ap328:2.1_0": "Siddhatthaṁ sabbasiddhatthaṁ,",
  "tha-ap328:2.2_0": "anejaṁ aparājitaṁ;",
  "tha-ap328:2.3_0": "Sambuddhaṁ atināmetvā,",
  "tha-ap328:2.4_0": "bhojayiṁ taṁ mahāmuniṁ.",
  "tha-ap328:3.1_0": "Muni kāruṇiko loke,",
  "tha-ap328:3.2_0": "obhāsayi mamaṁ tadā;",
  "tha-ap328:3.3_0": "Buddhe cittaṁ pasādetvā,",
  "tha-ap328:3.4_0": "kappaṁ saggamhi modahaṁ.",
  "tha-ap328:4.1_0": "Catunnavutito kappe,",
  "tha-ap328:4.2_0": "yaṁ dānamadadiṁ tadā;",
  "tha-ap328:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap328:4.4_0": "bhikkhādānassidaṁ phalaṁ.",
  "tha-ap328:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap328:5.2_0": "…pe…",
  "tha-ap328:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap328:6.1_0": "Itthaṁ sudaṁ āyasmā annasaṁsāvako thero imā gāthāyo abhāsitthāti.",
  "tha-ap328:7.1_0": "Annasaṁsāvakattherassāpadānaṁ chaṭṭhaṁ."
}