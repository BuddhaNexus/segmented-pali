{
  "tha-ap178:0.1_0": "Therāpadāna",
  "tha-ap178:0.2_0": "Kumudavagga",
  "tha-ap178:0.3_0": "6. Maggadattikattheraapadāna",
  "tha-ap178:1.1_0": "“Anomadassī bhagavā,",
  "tha-ap178:1.2_0": "dvipadindo narāsabho;",
  "tha-ap178:1.3_0": "Diṭṭhadhammasukhatthāya,",
  "tha-ap178:1.4_0": "abbhokāsamhi caṅkami.",
  "tha-ap178:2.1_0": "Uddhate pāde pupphāni,",
  "tha-ap178:2.2_0": "sobhaṁ muddhani tiṭṭhare;",
  "tha-ap178:2.3_0": "Pasannacitto sumano,",
  "tha-ap178:2.4_0": "vanditvā pupphamokiriṁ.",
  "tha-ap178:3.1_0": "Vīsakappasahassamhi,",
  "tha-ap178:3.2_0": "ito pañca janā ahuṁ;",
  "tha-ap178:3.3_0": "Pupphacchadaniyā nāma,",
  "tha-ap178:3.4_0": "cakkavattī mahabbalā.",
  "tha-ap178:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap178:4.2_0": "…pe…",
  "tha-ap178:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap178:5.1_0": "Itthaṁ sudaṁ āyasmā maggadattiko thero imā gāthāyo abhāsitthāti.",
  "tha-ap178:6.1_0": "Maggadattikattherassāpadānaṁ chaṭṭhaṁ."
}