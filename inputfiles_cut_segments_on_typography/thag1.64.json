{
  "thag1.64:0.1_0": "Theragāthā 1.64",
  "thag1.64:0.2_0": "Ekakanipāta",
  "thag1.64:0.3_0": "Sattamavagga",
  "thag1.64:0.4_0": "Vimalakoṇḍaññattheragāthā",
  "thag1.64:1.1_0": "“Dumavhayāya uppanno,",
  "thag1.64:1.2_0": "jāto paṇḍaraketunā;",
  "thag1.64:1.3_0": "Ketuhā ketunāyeva,",
  "thag1.64:1.4_0": "mahāketuṁ padhaṁsayī”ti.",
  "thag1.64:1.5_0": "… Vimalakoṇḍañño thero …."
}