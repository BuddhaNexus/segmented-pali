{
  "thag1.102:0.1_0": "Theragāthā 1.102",
  "thag1.102:0.2_0": "Ekakanipāta",
  "thag1.102:0.3_0": "Ekādasamavagga",
  "thag1.102:0.4_0": "Setucchattheragāthā",
  "thag1.102:1.1_0": "“Mānena vañcitāse,",
  "thag1.102:1.2_0": "Saṅkhāresu saṅkilissamānāse;",
  "thag1.102:1.3_0": "Lābhālābhena mathitā,",
  "thag1.102:1.4_0": "Samādhiṁ nādhigacchantī”ti.",
  "thag1.102:1.5_0": "… Setuccho thero …."
}