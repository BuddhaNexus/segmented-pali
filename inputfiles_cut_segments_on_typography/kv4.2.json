{
  "kv4.2:0.1_0": "Kathāvatthu",
  "kv4.2:0.2_0": "Mahāpaṇṇāsaka",
  "kv4.2:0.3_0": "Catutthavagga",
  "kv4.2:0.4_0": "Upapattikathā",
  "kv4.2:1.1_0": "Saha upapattiyā arahāti?",
  "kv4.2:1.2_0": "Āmantā.",
  "kv4.2:1.3_0": "Saha upapattiyā sotāpanno hotīti?",
  "kv4.2:1.4_0": "Na hevaṁ vattabbe …pe….",
  "kv4.2:2.1_0": "Saha upapattiyā arahāti?",
  "kv4.2:2.2_0": "Āmantā.",
  "kv4.2:2.3_0": "Saha upapattiyā sakadāgāmī hotīti?",
  "kv4.2:2.4_0": "Na hevaṁ vattabbe …pe….",
  "kv4.2:3.1_0": "Saha upapattiyā arahāti?",
  "kv4.2:3.2_0": "Āmantā.",
  "kv4.2:3.3_0": "Saha upapattiyā anāgāmī hotīti?",
  "kv4.2:3.4_0": "Na hevaṁ vattabbe …pe….",
  "kv4.2:4.1_0": "Saha upapattiyā sotāpanno na hotīti?",
  "kv4.2:4.2_0": "Āmantā.",
  "kv4.2:4.3_0": "Hañci saha upapattiyā sotāpanno na hoti, no ca vata re vattabbe—",
  "kv4.2:4.4_0": "“saha upapattiyā arahā”ti.",
  "kv4.2:5.1_0": "Saha upapattiyā sakadāgāmī na hotīti?",
  "kv4.2:5.2_0": "Āmantā.",
  "kv4.2:5.3_0": "Hañci saha upapattiyā sakadāgāmī na hoti, no ca vata re vattabbe—",
  "kv4.2:5.4_0": "“saha upapattiyā arahā”ti.",
  "kv4.2:6.1_0": "Saha upapattiyā anāgāmī na hotīti?",
  "kv4.2:6.2_0": "Āmantā.",
  "kv4.2:6.3_0": "Hañci saha upapattiyā anāgāmī na hoti, no ca vata re vattabbe—",
  "kv4.2:6.4_0": "“saha upapattiyā arahā”ti.",
  "kv4.2:7.1_0": "Saha upapattiyā arahāti?",
  "kv4.2:7.2_0": "Āmantā.",
  "kv4.2:7.3_0": "Sāriputto thero saha upapattiyā arahāti?",
  "kv4.2:7.4_0": "Na hevaṁ vattabbe.",
  "kv4.2:7.5_0": "Mahāmoggallāno thero …pe…",
  "kv4.2:7.6_0": "mahākassapo thero …pe…",
  "kv4.2:7.7_0": "mahākaccāno thero …pe…",
  "kv4.2:7.8_0": "mahākoṭṭhiko thero …pe…",
  "kv4.2:7.9_0": "mahāpanthako thero saha upapattiyā arahāti?",
  "kv4.2:7.10_0": "Na hevaṁ vattabbe …pe….",
  "kv4.2:8.1_0": "Sāriputto thero na saha upapattiyā arahāti?",
  "kv4.2:8.2_0": "Āmantā.",
  "kv4.2:8.3_0": "Hañci sāriputto thero na saha upapattiyā arahā, no ca vata re vattabbe—",
  "kv4.2:8.4_0": "“saha upapattiyā arahā”ti.",
  "kv4.2:9.1_0": "Mahāmoggallāno thero …pe…",
  "kv4.2:9.2_0": "mahākassapo thero …pe…",
  "kv4.2:9.3_0": "mahākaccāno thero …pe…",
  "kv4.2:9.4_0": "mahākoṭṭhiko thero …pe…",
  "kv4.2:9.5_0": "mahāpanthako thero na saha upapattiyā arahāti?",
  "kv4.2:9.6_0": "Āmantā.",
  "kv4.2:9.7_0": "Hañci mahāpanthako thero na saha upapattiyā arahā, no ca vata re vattabbe—",
  "kv4.2:9.8_0": "“saha upapattiyā arahā”ti.",
  "kv4.2:10.1_0": "Saha upapattiyā arahāti?",
  "kv4.2:10.2_0": "Āmantā.",
  "kv4.2:10.3_0": "Upapattesiyena cittena arahattaṁ sacchikaroti lokiyena sāsavena …pe… saṅkilesiyenāti?",
  "kv4.2:10.4_0": "Na hevaṁ vattabbe …pe….",
  "kv4.2:11.1_0": "Saha upapattiyā arahāti?",
  "kv4.2:11.2_0": "Āmantā.",
  "kv4.2:11.3_0": "Upapattesiyaṁ cittaṁ niyyānikaṁ khayagāmī bodhagāmī apacayagāmī anāsavaṁ …pe…",
  "kv4.2:11.4_0": "asaṅkilesiyanti?",
  "kv4.2:11.5_0": "Na hevaṁ vattabbe …pe….",
  "kv4.2:12.1_0": "Nanu upapattesiyaṁ cittaṁ aniyyānikaṁ na khayagāmi na bodhagāmi na apacayagāmi sāsavaṁ …pe…",
  "kv4.2:12.2_0": "saṅkilesiyanti?",
  "kv4.2:12.3_0": "Āmantā.",
  "kv4.2:12.4_0": "Hañci upapattesiyaṁ cittaṁ aniyyānikaṁ na khayagāmi na bodhagāmi na apacayagāmi sāsavaṁ …pe…",
  "kv4.2:12.4_1": "saṅkilesiyaṁ,",
  "kv4.2:12.4_2": "no ca vata re vattabbe—",
  "kv4.2:12.5_0": "“saha upapattiyā arahā”ti.",
  "kv4.2:13.1_0": "Saha upapattiyā arahāti?",
  "kv4.2:13.2_0": "Āmantā.",
  "kv4.2:13.3_0": "Upapattesiyena cittena rāgaṁ pajahati, dosaṁ pajahati, mohaṁ pajahati, anottappaṁ pajahatīti?",
  "kv4.2:13.4_0": "Na hevaṁ vattabbe …pe….",
  "kv4.2:14.1_0": "Saha upapattiyā arahāti?",
  "kv4.2:14.2_0": "Āmantā.",
  "kv4.2:14.3_0": "Upapattesiyaṁ cittaṁ maggo …",
  "kv4.2:14.3_1": "satipaṭṭhānaṁ …pe…",
  "kv4.2:14.3_2": "sammappadhānaṁ …",
  "kv4.2:14.3_3": "iddhipādo …",
  "kv4.2:14.3_4": "indriyaṁ … balaṁ …",
  "kv4.2:14.3_5": "bojjhaṅgoti?",
  "kv4.2:14.4_0": "Na hevaṁ vattabbe …pe….",
  "kv4.2:15.1_0": "Saha upapattiyā arahāti?",
  "kv4.2:15.2_0": "Āmantā.",
  "kv4.2:15.3_0": "Upapattesiyena cittena dukkhaṁ parijānāti,",
  "kv4.2:15.3_1": "samudayaṁ pajahati,",
  "kv4.2:15.3_2": "nirodhaṁ sacchikaroti,",
  "kv4.2:15.3_3": "maggaṁ bhāvetīti?",
  "kv4.2:15.4_0": "Na hevaṁ vattabbe …pe…",
  "kv4.2:15.5_0": "saha upapattiyā arahāti?",
  "kv4.2:15.6_0": "Āmantā.",
  "kv4.2:15.7_0": "Cuticittaṁ maggacittaṁ upapattesiyaṁ cittaṁ phalacittanti?",
  "kv4.2:15.8_0": "Na hevaṁ vattabbe …pe….",
  "kv4.2:15.9_0": "Upapattikathā niṭṭhitā."
}