{
  "tika-s0402t45:0_0": "(12) 2. Kesivaggo",
  "tika-s0402t45:1_0": "1-7. Kesisuttādivaṇṇanā",
  "tika-s0402t45:2_0": "111- 117.",
  "tika-s0402t45:2_1": "Dutiyassa paṭhame assadammeti dammanayogge asse.",
  "tika-s0402t45:2_2": "Sāretīti sikkhāpeti pavatteti.",
  "tika-s0402t45:2_3": "Sesamettha uttānameva.",
  "tika-s0402t45:2_4": "Dutiyādīni uttānatthāneva.",
  "tika-s0402t45:3_0": "Kesisuttādivaṇṇanā niṭṭhitā.",
  "tika-s0402t45:4_0": "8-10. Saṁvejanīyasuttādivaṇṇanā",
  "tika-s0402t45:5_0": "118- 120.",
  "tika-s0402t45:5_1": "Aṭṭhame saddhassāti buddhādīsu pasannacittassa vattasampannassa,",
  "tika-s0402t45:5_2": "yassa pāto vuṭṭhāya cetiyaṅgaṇavattādīni sabbavattāni katāneva paññāyanti.",
  "tika-s0402t45:5_3": "Dassanīyānīti dassanārahāni.",
  "tika-s0402t45:5_4": "Saṁvego nāma sahottappaṁ ñāṇaṁ.",
  "tika-s0402t45:5_5": "Abhijātiṭṭhānādīnipi tassa uppattihetūni bhavantīti sikkhāpento āha “saṁvegajanakānī”ti.",
  "tika-s0402t45:5_6": "Ṭhānānīti kāraṇāni,",
  "tika-s0402t45:5_7": "padesaṭṭhānāni vā.",
  "tika-s0402t45:5_8": "Navamadasamāni suviññeyyāni.",
  "tika-s0402t45:6_0": "Saṁvejanīyasuttādivaṇṇanā niṭṭhitā.",
  "tika-s0402t45:7_0": "Kesivaggavaṇṇanā niṭṭhitā."
}