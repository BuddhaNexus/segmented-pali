{
  "iti11:0.1_0": "Itivuttaka 11",
  "iti11:0.2_0": "Ekakanipāta",
  "iti11:0.3_0": "Dutiyavagga",
  "iti11:0.4_0": "Mohapariññāsutta",
  "iti11:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti11:2.1_0": "“Mohaṁ, bhikkhave, anabhijānaṁ aparijānaṁ tattha cittaṁ avirājayaṁ appajahaṁ abhabbo dukkhakkhayāya.",
  "iti11:2.2_0": "Mohañca kho,",
  "iti11:2.2_1": "bhikkhave,",
  "iti11:2.2_2": "abhijānaṁ parijānaṁ tattha cittaṁ virājayaṁ pajahaṁ bhabbo dukkhakkhayāyā”ti.",
  "iti11:2.3_0": "Etamatthaṁ bhagavā avoca.",
  "iti11:2.4_0": "Tatthetaṁ iti vuccati:",
  "iti11:3.1_0": "“Yena mohena mūḷhāse,",
  "iti11:3.2_0": "sattā gacchanti duggatiṁ;",
  "iti11:3.3_0": "Taṁ mohaṁ sammadaññāya,",
  "iti11:3.4_0": "pajahanti vipassino;",
  "iti11:3.5_0": "Pahāya na punāyanti,",
  "iti11:3.6_0": "imaṁ lokaṁ kudācanan”ti.",
  "iti11:4.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti11:4.2_0": "Paṭhamaṁ."
}