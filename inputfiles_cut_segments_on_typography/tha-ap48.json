{
  "tha-ap48:0.1_0": "Therāpadāna",
  "tha-ap48:0.2_0": "Upālivagga",
  "tha-ap48:0.3_0": "6. Padumacchadaniyattheraapadāna",
  "tha-ap48:1.1_0": "“Nibbute lokanāthamhi,",
  "tha-ap48:1.2_0": "vipassimhaggapuggale;",
  "tha-ap48:1.3_0": "Suphullapadumaṁ gayha,",
  "tha-ap48:1.4_0": "citamāropayiṁ ahaṁ.",
  "tha-ap48:2.1_0": "Āropite ca citake,",
  "tha-ap48:2.2_0": "vehāsaṁ nabhamuggami;",
  "tha-ap48:2.3_0": "Ākāse chadanaṁ katvā,",
  "tha-ap48:2.4_0": "citakamhi adhārayi.",
  "tha-ap48:3.1_0": "Ekanavutito kappe,",
  "tha-ap48:3.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap48:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap48:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap48:4.1_0": "Sattatālīsito kappe,",
  "tha-ap48:4.2_0": "padumissaranāmako;",
  "tha-ap48:4.3_0": "Cāturanto vijitāvī,",
  "tha-ap48:4.4_0": "cakkavattī mahabbalo.",
  "tha-ap48:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap48:5.2_0": "…pe…",
  "tha-ap48:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap48:6.1_0": "Itthaṁ sudaṁ āyasmā padumacchadaniyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap48:7.1_0": "Padumacchadaniyattherassāpadānaṁ chaṭṭhaṁ."
}