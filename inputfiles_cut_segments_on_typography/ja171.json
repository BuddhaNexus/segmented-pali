{
  "ja171:0.1_0": "Jātaka",
  "ja171:0.2_0": "Dukanipāta",
  "ja171:0.3_0": "Kalyāṇavagga",
  "ja171:0.4_0": "1. Kalyāṇadhammajātaka",
  "ja171:1.1_0": "“Kalyāṇadhammoti yadā janinda,",
  "ja171:1.2_0": "Loke samaññaṁ anupāpuṇāti;",
  "ja171:1.3_0": "Tasmā na hiyyetha naro sapañño,",
  "ja171:1.4_0": "Hiriyāpi santo dhuramādiyanti.",
  "ja171:2.1_0": "Sāyaṁ samaññā idha majja pattā,",
  "ja171:2.2_0": "Kalyāṇadhammoti janinda loke;",
  "ja171:2.3_0": "Tāhaṁ samekkhaṁ idha pabbajissaṁ,",
  "ja171:2.4_0": "Na hi matthi chando idha kāmabhoge”ti.",
  "ja171:3.1_0": "Kalyāṇadhammajātakaṁ paṭhamaṁ."
}