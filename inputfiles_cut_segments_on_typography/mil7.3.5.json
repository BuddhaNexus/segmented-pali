{
  "mil7.3.5:0.1_0": "Milindapañha",
  "mil7.3.5:0.2_0": "Opammakathāpañha",
  "mil7.3.5:0.3_0": "Samuddavagga",
  "mil7.3.5:0.4_0": "5. Nāvaṅgapañha",
  "mil7.3.5:1.1_0": "“Bhante nāgasena,",
  "mil7.3.5:1.1_1": "‘nāvāya tīṇi aṅgāni gahetabbānī’ti yaṁ vadesi,",
  "mil7.3.5:1.1_2": "katamāni tāni tīṇi aṅgāni gahetabbānī”ti?",
  "mil7.3.5:1.2_0": "“Yathā, mahārāja, nāvā bahuvidhadārusaṅghāṭasamavāyena bahumpi janaṁ tārayati;",
  "mil7.3.5:1.3_0": "evameva kho, mahārāja,",
  "mil7.3.5:1.3_1": "yoginā yogāvacarena ācārasīlaguṇavattappaṭivattabahuvidhadhammasaṅghāṭasamavāyena sadevako loko tārayitabbo.",
  "mil7.3.5:1.4_0": "Idaṁ, mahārāja, nāvāya paṭhamaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.3.5:2.1_0": "Puna caparaṁ, mahārāja, nāvā bahuvidhaūmitthanitavegavisaṭamāvaṭṭavegaṁ sahati;",
  "mil7.3.5:2.2_0": "evameva kho, mahārāja,",
  "mil7.3.5:2.2_1": "yoginā yogāvacarena bahuvidhakilesaūmivegaṁ lābhasakkārayasasilokapūjanavandanā parakulesu nindāpasaṁsāsukhadukkhasammānanavimānanabahuvidhadosaūmivegañca",
  "mil7.3.5:2.2_2": "sahitabbaṁ.",
  "mil7.3.5:2.3_0": "Idaṁ, mahārāja, nāvāya dutiyaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.3.5:3.1_0": "Puna caparaṁ, mahārāja,",
  "mil7.3.5:3.1_1": "nāvā aparimitamanantamapāramakkhobhitagambhīre mahatimahāghose timitimiṅgalamakaramacchagaṇākule mahatimahāsamudde carati;",
  "mil7.3.5:3.2_0": "evameva kho, mahārāja,",
  "mil7.3.5:3.2_1": "yoginā yogāvacarena tiparivaṭṭadvādasākāracatusaccābhisamayappaṭivedhe mānasaṁ sañcārayitabbaṁ.",
  "mil7.3.5:3.3_0": "Idaṁ, mahārāja, nāvāya tatiyaṁ aṅgaṁ gahetabbaṁ.",
  "mil7.3.5:3.4_0": "Bhāsitampetaṁ, mahārāja, bhagavatā devātidevena saṁyuttanikāyavare saccasaṁyutte—",
  "mil7.3.5:4.1_0": "‘Vitakkentā ca kho tumhe,",
  "mil7.3.5:4.1_1": "bhikkhave,",
  "mil7.3.5:4.1_2": "“idaṁ dukkhan”ti vitakkeyyātha,",
  "mil7.3.5:4.1_3": "“ayaṁ dukkhasamudayo”ti vitakkeyyātha,",
  "mil7.3.5:4.1_4": "“ayaṁ dukkhanirodho”ti vitakkeyyātha,",
  "mil7.3.5:4.1_5": "“ayaṁ dukkhanirodhagāminī paṭipadā”ti vitakkeyyāthā’”ti.",
  "mil7.3.5:5.1_0": "Nāvaṅgapañho pañcamo."
}