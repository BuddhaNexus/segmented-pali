{
  "pli-tv-pvr20:0.1_0": "Parivāra",
  "pli-tv-pvr20:0.2_0": "Sedamocanagāthā",
  "pli-tv-pvr20:0.3_0": "1. Avippavāsapañhā",
  "pli-tv-pvr20:1.1_0": "Asaṁvāso bhikkhūhi ca bhikkhunīhi ca,",
  "pli-tv-pvr20:1.2_0": "Sambhogo ekacco tahiṁ na labbhati;",
  "pli-tv-pvr20:1.3_0": "Avippavāsena anāpatti,",
  "pli-tv-pvr20:1.4_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:2.1_0": "Avissajjiyaṁ avebhaṅgiyaṁ,",
  "pli-tv-pvr20:2.2_0": "Pañca vuttā mahesinā;",
  "pli-tv-pvr20:2.3_0": "Vissajjantassa paribhuñjantassa anāpatti,",
  "pli-tv-pvr20:2.4_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:3.1_0": "Dasa puggale na vadāmi,",
  "pli-tv-pvr20:3.2_0": "Ekādasa vivajjiya;",
  "pli-tv-pvr20:3.3_0": "Vuḍḍhaṁ vandantassa āpatti,",
  "pli-tv-pvr20:3.4_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:4.1_0": "Na ukkhittako na ca pana pārivāsiko,",
  "pli-tv-pvr20:4.2_0": "Na saṅghabhinno na ca pana pakkhasaṅkanto;",
  "pli-tv-pvr20:4.3_0": "Samānasaṁvāsakabhūmiyā ṭhito,",
  "pli-tv-pvr20:4.4_0": "Kathaṁ nu sikkhāya asādhāraṇo siyā;",
  "pli-tv-pvr20:4.5_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:5.1_0": "Upeti dhammaṁ paripucchamāno,",
  "pli-tv-pvr20:5.2_0": "Kusalaṁ atthūpasañhitaṁ;",
  "pli-tv-pvr20:5.3_0": "Na jīvati na mato na nibbuto,",
  "pli-tv-pvr20:5.4_0": "Taṁ puggalaṁ katamaṁ vadanti buddhā;",
  "pli-tv-pvr20:5.5_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:6.1_0": "Ubbhakkhake na vadāmi,",
  "pli-tv-pvr20:6.2_0": "Adho nābhiṁ vivajjiya;",
  "pli-tv-pvr20:6.3_0": "Methunadhammapaccayā,",
  "pli-tv-pvr20:6.4_0": "Kathaṁ pārājiko siyā;",
  "pli-tv-pvr20:6.5_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:7.1_0": "Bhikkhu saññācikāya kuṭiṁ karoti,",
  "pli-tv-pvr20:7.2_0": "Adesitavatthukaṁ pamāṇātikkantaṁ;",
  "pli-tv-pvr20:7.3_0": "Sārambhaṁ aparikkamanaṁ anāpatti,",
  "pli-tv-pvr20:7.4_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:8.1_0": "Bhikkhu saññācikāya kuṭiṁ karoti,",
  "pli-tv-pvr20:8.2_0": "Desitavatthukaṁ pamāṇikaṁ;",
  "pli-tv-pvr20:8.3_0": "Anārambhaṁ saparikkamanaṁ āpatti,",
  "pli-tv-pvr20:8.4_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:9.1_0": "Na kāyikaṁ kiñci payogamācare,",
  "pli-tv-pvr20:9.2_0": "Na cāpi vācāya pare bhaṇeyya;",
  "pli-tv-pvr20:9.3_0": "Āpajjeyya garukaṁ chejjavatthuṁ,",
  "pli-tv-pvr20:9.4_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:10.1_0": "Na kāyikaṁ vācasikañca kiñci,",
  "pli-tv-pvr20:10.2_0": "Manasāpi santo na kareyya pāpaṁ;",
  "pli-tv-pvr20:10.3_0": "So nāsito kinti sunāsito bhave,",
  "pli-tv-pvr20:10.4_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:11.1_0": "Anālapanto manujena kenaci,",
  "pli-tv-pvr20:11.2_0": "Vācāgiraṁ no ca pare bhaṇeyya;",
  "pli-tv-pvr20:11.3_0": "Āpajjeyya vācasikaṁ na kāyikaṁ,",
  "pli-tv-pvr20:11.4_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:12.1_0": "Sikkhāpadā buddhavarena vaṇṇitā,",
  "pli-tv-pvr20:12.2_0": "Saṅghādisesā caturo bhaveyyuṁ;",
  "pli-tv-pvr20:12.3_0": "Āpajjeyya ekapayogena sabbe,",
  "pli-tv-pvr20:12.4_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:13.1_0": "Ubho ekato upasampannā,",
  "pli-tv-pvr20:13.2_0": "Ubhinnaṁ hatthato cīvaraṁ paṭiggaṇheyya;",
  "pli-tv-pvr20:13.3_0": "Siyā āpattiyo nānā,",
  "pli-tv-pvr20:13.4_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:14.1_0": "Caturo janā saṁvidhāya,",
  "pli-tv-pvr20:14.2_0": "Garubhaṇḍaṁ avāharuṁ;",
  "pli-tv-pvr20:14.3_0": "Tayo pārājikā eko na pārājiko,",
  "pli-tv-pvr20:14.4_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:15.0_0": "2. Pārājikādipañhā",
  "pli-tv-pvr20:15.1_0": "Itthī ca abbhantare siyā,",
  "pli-tv-pvr20:15.2_0": "Bhikkhu ca bahiddhā siyā;",
  "pli-tv-pvr20:15.3_0": "Chiddaṁ tasmiṁ ghare natthi,",
  "pli-tv-pvr20:15.4_0": "Methunadhammapaccayā;",
  "pli-tv-pvr20:15.5_0": "Kathaṁ pārājiko siyā,",
  "pli-tv-pvr20:15.6_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:16.1_0": "Telaṁ madhuṁ phāṇitañcāpi sappiṁ,",
  "pli-tv-pvr20:16.2_0": "Sāmaṁ gahetvāna nikkhipeyya;",
  "pli-tv-pvr20:16.3_0": "Avītivatte sattāhe,",
  "pli-tv-pvr20:16.4_0": "Sati paccaye paribhuñjantassa āpatti;",
  "pli-tv-pvr20:16.5_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:17.1_0": "Nissaggiyena āpatti,",
  "pli-tv-pvr20:17.2_0": "Suddhakena pācittiyaṁ;",
  "pli-tv-pvr20:17.3_0": "Āpajjantassa ekato,",
  "pli-tv-pvr20:17.4_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:18.1_0": "Bhikkhū siyā vīsatiyā samāgatā,",
  "pli-tv-pvr20:18.2_0": "Kammaṁ kareyyuṁ samaggasaññino;",
  "pli-tv-pvr20:18.3_0": "Bhikkhu siyā dvādasayojane ṭhito,",
  "pli-tv-pvr20:18.4_0": "Kammañca taṁ kuppeyya vaggapaccayā;",
  "pli-tv-pvr20:18.5_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:19.1_0": "Padavītihāramattena vācāya bhaṇitena ca,",
  "pli-tv-pvr20:19.2_0": "Sabbāni garukāni sappaṭikammāni;",
  "pli-tv-pvr20:19.3_0": "Catusaṭṭhi āpattiyo āpajjeyya ekato,",
  "pli-tv-pvr20:19.4_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:20.1_0": "Nivattho antaravāsakena,",
  "pli-tv-pvr20:20.2_0": "Diguṇaṁ saṅghāṭiṁ pāruto;",
  "pli-tv-pvr20:20.3_0": "Sabbāni tāni nissaggiyāni honti,",
  "pli-tv-pvr20:20.4_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:21.1_0": "Na cāpi ñatti na ca pana kammavācā,",
  "pli-tv-pvr20:21.2_0": "Na cehi bhikkhūti jino avoca;",
  "pli-tv-pvr20:21.3_0": "Saraṇagamanampi na tassa atthi,",
  "pli-tv-pvr20:21.4_0": "Upasampadā cassa akuppā;",
  "pli-tv-pvr20:21.5_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:22.1_0": "Itthiṁ hane na mātaraṁ,",
  "pli-tv-pvr20:22.2_0": "Purisañca na pitaraṁ hane;",
  "pli-tv-pvr20:22.3_0": "Haneyya anariyaṁ mando,",
  "pli-tv-pvr20:22.4_0": "Tena cānantaraṁ phuse;",
  "pli-tv-pvr20:22.5_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:23.1_0": "Itthiṁ hane ca mātaraṁ,",
  "pli-tv-pvr20:23.2_0": "Purisañca pitaraṁ hane;",
  "pli-tv-pvr20:23.3_0": "Mātaraṁ pitaraṁ hantvā,",
  "pli-tv-pvr20:23.4_0": "Na tenānantaraṁ phuse;",
  "pli-tv-pvr20:23.5_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:24.1_0": "Acodayitvā assārayitvā,",
  "pli-tv-pvr20:24.2_0": "Asammukhībhūtassa kareyya kammaṁ;",
  "pli-tv-pvr20:24.3_0": "Katañca kammaṁ sukataṁ bhaveyya,",
  "pli-tv-pvr20:24.4_0": "Kārako ca saṅgho anāpattiko siyā;",
  "pli-tv-pvr20:24.5_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:25.1_0": "Codayitvā sārayitvā,",
  "pli-tv-pvr20:25.2_0": "Sammukhībhūtassa kareyya kammaṁ;",
  "pli-tv-pvr20:25.3_0": "Katañca kammaṁ akataṁ bhaveyya,",
  "pli-tv-pvr20:25.4_0": "Kārako ca saṅgho sāpattiko siyā;",
  "pli-tv-pvr20:25.5_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:26.1_0": "Chindantassa āpatti,",
  "pli-tv-pvr20:26.2_0": "Chindantassa anāpatti;",
  "pli-tv-pvr20:26.3_0": "Chādentassa āpatti,",
  "pli-tv-pvr20:26.4_0": "Chādentassa anāpatti;",
  "pli-tv-pvr20:26.5_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:27.1_0": "Saccaṁ bhaṇanto garukaṁ,",
  "pli-tv-pvr20:27.2_0": "Musā ca lahu bhāsato;",
  "pli-tv-pvr20:27.3_0": "Musā bhaṇanto garukaṁ,",
  "pli-tv-pvr20:27.4_0": "Saccañca lahu bhāsato;",
  "pli-tv-pvr20:27.5_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:28.0_0": "3. Pācittiyādipañhā",
  "pli-tv-pvr20:28.1_0": "Adhiṭṭhitaṁ rajanāya rattaṁ,",
  "pli-tv-pvr20:28.2_0": "Kappakatampi santaṁ;",
  "pli-tv-pvr20:28.3_0": "Paribhuñjantassa āpatti,",
  "pli-tv-pvr20:28.4_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:29.1_0": "Atthaṅgate sūriye bhikkhu maṁsāni khādati,",
  "pli-tv-pvr20:29.2_0": "Na ummattako na ca pana khittacitto;",
  "pli-tv-pvr20:29.3_0": "Na cāpi so vedanāṭṭo bhaveyya,",
  "pli-tv-pvr20:29.4_0": "Na cassa hoti āpatti;",
  "pli-tv-pvr20:29.5_0": "So ca dhammo sugatena desito,",
  "pli-tv-pvr20:29.6_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:30.1_0": "Na rattacitto na ca pana theyyacitto,",
  "pli-tv-pvr20:30.2_0": "Na cāpi so paraṁ maraṇāya cetayi;",
  "pli-tv-pvr20:30.3_0": "Salākaṁ dentassa hoti chejjaṁ,",
  "pli-tv-pvr20:30.4_0": "Paṭiggaṇhantassa thullaccayaṁ;",
  "pli-tv-pvr20:30.5_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:31.1_0": "Na cāpi āraññakaṁ sāsaṅkasammataṁ,",
  "pli-tv-pvr20:31.2_0": "Na cāpi saṅghena sammuti dinnā;",
  "pli-tv-pvr20:31.3_0": "Na cassa kathinaṁ atthataṁ tattheva,",
  "pli-tv-pvr20:31.4_0": "Cīvaraṁ nikkhipitvā gaccheyya aḍḍhayojanaṁ;",
  "pli-tv-pvr20:31.5_0": "Tattheva aruṇaṁ uggacchantassa anāpatti,",
  "pli-tv-pvr20:31.6_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:32.1_0": "Kāyikāni na vācasikāni,",
  "pli-tv-pvr20:32.2_0": "Sabbāni nānāvatthukāni;",
  "pli-tv-pvr20:32.3_0": "Apubbaṁ acarimaṁ āpajjeyya ekato,",
  "pli-tv-pvr20:32.4_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:33.1_0": "Vācasikāni na kāyikāni,",
  "pli-tv-pvr20:33.2_0": "Sabbāni nānāvatthukāni;",
  "pli-tv-pvr20:33.3_0": "Apubbaṁ acarimaṁ āpajjeyya ekato,",
  "pli-tv-pvr20:33.4_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:34.1_0": "Tissitthiyo methunaṁ taṁ na seve,",
  "pli-tv-pvr20:34.2_0": "Tayo purise tayo anariyapaṇḍake;",
  "pli-tv-pvr20:34.3_0": "Na cācare methunaṁ byañjanasmiṁ,",
  "pli-tv-pvr20:34.4_0": "Chejjaṁ siyā methunadhammapaccayā;",
  "pli-tv-pvr20:34.5_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:35.1_0": "Mātaraṁ cīvaraṁ yāce,",
  "pli-tv-pvr20:35.2_0": "No ca saṅghe pariṇataṁ;",
  "pli-tv-pvr20:35.3_0": "Kenassa hoti āpatti,",
  "pli-tv-pvr20:35.4_0": "Anāpatti ca ñātake;",
  "pli-tv-pvr20:35.5_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:36.1_0": "Kuddho ārādhako hoti,",
  "pli-tv-pvr20:36.2_0": "Kuddho hoti garahiyo;",
  "pli-tv-pvr20:36.3_0": "Atha ko nāma so dhammo,",
  "pli-tv-pvr20:36.4_0": "Yena kuddho pasaṁsiyo;",
  "pli-tv-pvr20:36.5_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:37.1_0": "Tuṭṭho ārādhako hoti,",
  "pli-tv-pvr20:37.2_0": "Tuṭṭho hoti garahiyo;",
  "pli-tv-pvr20:37.3_0": "Atha ko nāma so dhammo,",
  "pli-tv-pvr20:37.4_0": "Yena tuṭṭho garahiyo;",
  "pli-tv-pvr20:37.5_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:38.1_0": "Saṅghādisesaṁ thullaccayaṁ,",
  "pli-tv-pvr20:38.2_0": "Pācittiyaṁ pāṭidesanīyaṁ;",
  "pli-tv-pvr20:38.3_0": "Dukkaṭaṁ āpajjeyya ekato,",
  "pli-tv-pvr20:38.4_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:39.1_0": "Ubho paripuṇṇavīsativassā,",
  "pli-tv-pvr20:39.2_0": "Ubhinnaṁ ekupajjhāyo;",
  "pli-tv-pvr20:39.3_0": "Ekācariyo ekā kammavācā,",
  "pli-tv-pvr20:39.4_0": "Eko upasampanno eko anupasampanno;",
  "pli-tv-pvr20:39.5_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:40.1_0": "Akappakataṁ nāpi rajanāya rattaṁ,",
  "pli-tv-pvr20:40.2_0": "Tena nivattho yena kāmaṁ vajeyya;",
  "pli-tv-pvr20:40.3_0": "Na cassa hoti āpatti,",
  "pli-tv-pvr20:40.4_0": "So ca dhammo sugatena desito;",
  "pli-tv-pvr20:40.5_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:41.1_0": "Na deti na paṭiggaṇhāti,",
  "pli-tv-pvr20:41.2_0": "Paṭiggaho tena na vijjati;",
  "pli-tv-pvr20:41.3_0": "Āpajjati garukaṁ na lahukaṁ,",
  "pli-tv-pvr20:41.4_0": "Tañca paribhogapaccayā;",
  "pli-tv-pvr20:41.5_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:42.1_0": "Na deti na paṭiggaṇhāti,",
  "pli-tv-pvr20:42.2_0": "Paṭiggaho tena na vijjati;",
  "pli-tv-pvr20:42.3_0": "Āpajjati lahukaṁ na garukaṁ,",
  "pli-tv-pvr20:42.4_0": "Tañca paribhogapaccayā;",
  "pli-tv-pvr20:42.5_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:43.1_0": "Āpajjati garukaṁ sāvasesaṁ,",
  "pli-tv-pvr20:43.2_0": "Chādeti anādariyaṁ paṭicca;",
  "pli-tv-pvr20:43.3_0": "Na bhikkhunī no ca phuseyya vajjaṁ,",
  "pli-tv-pvr20:43.4_0": "Pañhā mesā kusalehi cintitā.",
  "pli-tv-pvr20:44.1_0": "Sedamocanagāthā niṭṭhitā.",
  "pli-tv-pvr20:45.1_0": "Tassuddānaṁ",
  "pli-tv-pvr20:46.1_0": "Asaṁvāso avissajji,",
  "pli-tv-pvr20:46.2_0": "Dasa ca anukkhittako;",
  "pli-tv-pvr20:46.3_0": "Upeti dhammaṁ ubbhakkhakaṁ,",
  "pli-tv-pvr20:46.4_0": "Tato saññācikā ca dve.",
  "pli-tv-pvr20:47.1_0": "Na kāyikañca garukaṁ,",
  "pli-tv-pvr20:47.2_0": "na kāyikaṁ na vācasikaṁ;",
  "pli-tv-pvr20:47.3_0": "Anālapanto sikkhā ca,",
  "pli-tv-pvr20:47.4_0": "ubho ca caturo janā.",
  "pli-tv-pvr20:48.1_0": "Itthī telañca nissaggi,",
  "pli-tv-pvr20:48.2_0": "bhikkhu ca padavītiyo;",
  "pli-tv-pvr20:48.3_0": "Nivattho ca na ca ñatti,",
  "pli-tv-pvr20:48.4_0": "na mātaraṁ pitaraṁ hane.",
  "pli-tv-pvr20:49.1_0": "Acodayitvā codayitvā,",
  "pli-tv-pvr20:49.2_0": "chindantaṁ saccameva ca;",
  "pli-tv-pvr20:49.3_0": "Adhiṭṭhitañcatthaṅgate,",
  "pli-tv-pvr20:49.4_0": "na rattaṁ na cāraññakaṁ.",
  "pli-tv-pvr20:50.1_0": "Kāyikā vācasikā ca,",
  "pli-tv-pvr20:50.2_0": "tissitthī cāpi mātaraṁ;",
  "pli-tv-pvr20:50.3_0": "Kuddho ārādhako tuṭṭho,",
  "pli-tv-pvr20:50.4_0": "saṅghādisesā ca ubho.",
  "pli-tv-pvr20:51.1_0": "Akappakataṁ na deti,",
  "pli-tv-pvr20:51.2_0": "Na detāpajjatī garuṁ;",
  "pli-tv-pvr20:51.3_0": "Sedamocanikā gāthā,",
  "pli-tv-pvr20:51.4_0": "Pañhā viññūhi vibhāvitāti."
}