{
  "tha-ap554:0.1_0": "Therāpadāna",
  "tha-ap554:0.2_0": "Yasavagga",
  "tha-ap554:0.3_0": "2. Nadīkassapattheraapadāna",
  "tha-ap554:1.1_0": "“Padumuttarassa bhagavato,",
  "tha-ap554:1.2_0": "Lokajeṭṭhassa tādino;",
  "tha-ap554:1.3_0": "Piṇḍacāraṁ carantassa,",
  "tha-ap554:1.4_0": "Vārato uttamaṁ yasaṁ;",
  "tha-ap554:1.5_0": "Aggaphalaṁ gahetvāna,",
  "tha-ap554:1.6_0": "Adāsiṁ satthuno ahaṁ.",
  "tha-ap554:2.1_0": "Tena kammena devindo,",
  "tha-ap554:2.2_0": "lokajeṭṭho narāsabho;",
  "tha-ap554:2.3_0": "Sampattomhi acalaṁ ṭhānaṁ,",
  "tha-ap554:2.4_0": "hitvā jayaparājayaṁ.",
  "tha-ap554:3.1_0": "Satasahassito kappe,",
  "tha-ap554:3.2_0": "yaṁ phalaṁ adadiṁ tadā;",
  "tha-ap554:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap554:3.4_0": "aggadānassidaṁ phalaṁ.",
  "tha-ap554:4.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap554:4.2_0": "…pe…",
  "tha-ap554:4.3_0": "viharāmi anāsavo.",
  "tha-ap554:5.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap554:5.2_0": "…pe…",
  "tha-ap554:5.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap554:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap554:6.2_0": "…pe…",
  "tha-ap554:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap554:7.1_0": "Itthaṁ sudaṁ āyasmā nadīkassapo thero imā gāthāyo abhāsitthāti.",
  "tha-ap554:8.1_0": "Nadīkassapattherassāpadānaṁ dutiyaṁ."
}