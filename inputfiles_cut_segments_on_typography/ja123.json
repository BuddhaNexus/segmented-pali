{
  "ja123:0.1_0": "Jātaka",
  "ja123:0.2_0": "Ekakanipāta",
  "ja123:0.3_0": "Kusanāḷivagga",
  "ja123:0.4_0": "3. Naṅgalīsajātaka",
  "ja123:1.1_0": "“Asabbatthagāmiṁ vācaṁ,",
  "ja123:1.2_0": "Bālo sabbattha bhāsati;",
  "ja123:1.3_0": "Nāyaṁ dadhiṁ vedi na naṅgalīsaṁ,",
  "ja123:1.4_0": "Dadhippayaṁ maññati naṅgalīsan”ti.",
  "ja123:2.1_0": "Naṅgalīsajātakaṁ tatiyaṁ."
}