{
  "thag2.46:0.1_0": "Theragāthā 2.46",
  "thag2.46:0.2_0": "Dukanipāta",
  "thag2.46:0.3_0": "Pañcamavagga",
  "thag2.46:0.4_0": "Cūḷakattheragāthā",
  "thag2.46:1.1_0": "“Nadanti morā susikhā supekhuṇā,",
  "thag2.46:1.2_0": "Sunīlagīvā sumukhā sugajjino;",
  "thag2.46:1.3_0": "Susaddalā cāpi mahāmahī ayaṁ,",
  "thag2.46:1.4_0": "Subyāpitambu suvalāhakaṁ nabhaṁ.",
  "thag2.46:2.1_0": "Sukallarūpo sumanassa jhāyataṁ,",
  "thag2.46:2.2_0": "Sunikkamo sādhu subuddhasāsane;",
  "thag2.46:2.3_0": "Susukkasukkaṁ nipuṇaṁ sududdasaṁ,",
  "thag2.46:2.4_0": "Phusāhi taṁ uttamamaccutaṁ padan”ti.",
  "thag2.46:2.5_0": "… Cūḷako thero …."
}