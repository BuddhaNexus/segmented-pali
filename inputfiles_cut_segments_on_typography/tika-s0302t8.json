{
  "tika-s0302t8:0_0": "9. Opammasaṁyuttaṁ",
  "tika-s0302t8:1_0": "1. Kūṭasuttavaṇṇanā",
  "tika-s0302t8:2_0": "223.Kūṭaṁgacchantīti kūṭacchiddassa anupavisanavasena kūṭaṁ gacchanti.",
  "tika-s0302t8:2_1": "Yā ca gopānasiyo gopānasantaragatā,",
  "tika-s0302t8:2_2": "tāpi kūṭaṁ āhacca ṭhānena kūṭaṅgamā.",
  "tika-s0302t8:2_3": "Duvidhāpi kūṭe samosaraṇā.",
  "tika-s0302t8:2_4": "Kūṭassa samugghātena vināsena bhijjanena.",
  "tika-s0302t8:2_5": "Avijjāya samugghātenāti avijjāya accantameva appavattiyā.",
  "tika-s0302t8:2_6": "Tena ca mokkhadhammādhigamena tadanurūpadhammādhigamo dassito.",
  "tika-s0302t8:2_7": "Appamattāti pana iminā tassa upāyo dassito.",
  "tika-s0302t8:3_0": "Kūṭasuttavaṇṇanā niṭṭhitā.",
  "tika-s0302t8:4_0": "2. Nakhasikhasuttavaṇṇanā",
  "tika-s0302t8:5_0": "224.Evaṁ appakā yathā nakhasikhāya āropitapaṁsu,",
  "tika-s0302t8:5_1": "sugatisaṁvattaniyassa kammassa appakattā evaṁ devesupīti hīnūdāharaṇavasena vuttaṁ.",
  "tika-s0302t8:5_2": "Appatarā hi sattā ye devesu jāyanti,",
  "tika-s0302t8:5_3": "tañca kho kāmadevesu.",
  "tika-s0302t8:5_4": "Itaresu pana vattabbameva natthi.",
  "tika-s0302t8:6_0": "Nakhasikhasuttavaṇṇanā niṭṭhitā.",
  "tika-s0302t8:7_0": "3. Kulasuttavaṇṇanā",
  "tika-s0302t8:8_0": "225.Vidhaṁsayanti viheṭhayanti.",
  "tika-s0302t8:8_1": "Vaḍḍhitāti bhāvanāpāripūrivasena paribrūhitā.",
  "tika-s0302t8:8_2": "Punappunaṁ katāti bhāvanāya bahulīkaraṇena aparāparaṁ pavattitā yuttayānaṁ viya katāti yathā yuttaṁ ājaññarathaṁ",
  "tika-s0302t8:8_3": "chekena sārathinā adhiṭṭhitaṁ yathāruci pavattati,",
  "tika-s0302t8:8_4": "evaṁ yathāruci pavattiyā gamitā.",
  "tika-s0302t8:8_5": "Patiṭṭhānaṭṭhenāti adhiṭṭhānaṭṭhena.",
  "tika-s0302t8:8_6": "Vatthu viya katā sabbaso upakkilesavisodhanena suvisodhitamariyādaṁ viya katā.",
  "tika-s0302t8:8_7": "Adhiṭṭhitāti paṭipakkhadūrībhāvato subhāvitabhāvena avikampaneyyatāya ṭhapitā.",
  "tika-s0302t8:8_8": "Samantato citāti sabbabhāgena bhāvanūpacayaṁ gamitā.",
  "tika-s0302t8:8_9": "Tenāha “suvaḍḍhitā”ti.",
  "tika-s0302t8:8_10": "Suṭṭhu samāraddhāti mettābhāvanāya matthakappattiyā sammadeva sampāditā.",
  "tika-s0302t8:9_0": "Kulasuttavaṇṇanā niṭṭhitā.",
  "tika-s0302t8:10_0": "4. Okkhāsuttavaṇṇanā",
  "tika-s0302t8:11_0": "226.Mahāmukhaukkhalīnanti mahāmukhānaṁ mahantakoḷumbānaṁ sataṁ.",
  "tika-s0302t8:11_1": "Paṇītabhojanabharitānanti sappimadhusakkarādīhi upanītapaṇītabhojanehi paripuṇṇānaṁ.",
  "tika-s0302t8:11_2": "Tassāti pāṭhassa.",
  "tika-s0302t8:11_3": "Goduhanamattanti godohanavelāmattaṁ.",
  "tika-s0302t8:11_4": "Taṁ pana kittakaṁ adhippetanti āha “gāviyā”tiādi.",
  "tika-s0302t8:11_5": "Sabbasattesu hitapharaṇanti anodhisomettābhāvanamāha –",
  "tika-s0302t8:11_6": "mettacittaṁ appanāppattaṁ bhāvetuṁ sakkotīti adhippāyo.",
  "tika-s0302t8:11_7": "Tampi tato yathāvuttadānato mahapphalataranti.",
  "tika-s0302t8:12_0": "Okkhāsuttavaṇṇanā niṭṭhitā.",
  "tika-s0302t8:13_0": "5. Sattisuttavaṇṇanā",
  "tika-s0302t8:14_0": "227.Agge paharitvāti tiṇhaphalasattiyā agge hatthena vā muṭṭhinā vā pahāraṁ datvā.",
  "tika-s0302t8:14_1": "Kappāsavaṭṭiṁ viyāti pahatakappāsapiṇḍaṁ viya.",
  "tika-s0302t8:14_2": "Niyyāsavaṭṭiṁ viyāti phalasaṇṭhānaṁ niyyāsapiṇḍaṁ viya.",
  "tika-s0302t8:14_3": "Ekato katvāti kalikādibhāvena vīsatiṁsapiṇḍāni ekajjhaṁ katvā.",
  "tika-s0302t8:14_4": "Alliyāpento piṇḍaṁ karonto.",
  "tika-s0302t8:14_5": "Paṭileṇetīti paṭilīnayati nāmeti.",
  "tika-s0302t8:14_6": "Alliyāpento te dvepi dhārā ekato samphusāpento.",
  "tika-s0302t8:14_7": "Paṭikoṭṭetīti paṭipaharati.",
  "tika-s0302t8:14_8": "Tattha khaṇḍaṁ viya niyyāso.",
  "tika-s0302t8:14_9": "Kappāsavaṭṭanakaraṇīyanti vihatassa kappāsassa paṭisaṁharaṇavasena bandhanadaṇḍaṁ.",
  "tika-s0302t8:14_10": "Pavattentoti kappāsassa saṁvellanavasena pavattento.",
  "tika-s0302t8:15_0": "Sattisuttavaṇṇanā niṭṭhitā.",
  "tika-s0302t8:16_0": "6. Dhanuggahasuttavaṇṇanā",
  "tika-s0302t8:17_0": "228.Daḷhadhanunoti thirataradhanuno.",
  "tika-s0302t8:17_1": "Idāni tassa thiratarabhāvaṁ paricchedato dassetuṁ “daḷhadhanū”tiādi vuttaṁ.",
  "tika-s0302t8:17_2": "Tattha dvisahassathāmanti palānaṁ dvisahassathāmaṁ.",
  "tika-s0302t8:17_3": "Vuttamevatthaṁ pākaṭataraṁ katvā dassetuṁ “yassā”tiādimāha.",
  "tika-s0302t8:17_4": "Tattha yassāti dhanuno.",
  "tika-s0302t8:17_5": "Āropitassāti jiyaṁ āropitassa.",
  "tika-s0302t8:17_6": "Jiyābaddhoti jiyāya baddho.",
  "tika-s0302t8:17_7": "Pathavito muccati,",
  "tika-s0302t8:17_8": "etaṁ “dvisahassathāma”nti veditabbaṁ.",
  "tika-s0302t8:17_9": "Lohasīsādīnanti kāḷalohatambalohasīsādīnaṁ.",
  "tika-s0302t8:17_10": "Bhāroti purisabhāro,",
  "tika-s0302t8:17_11": "so pana majjhimapurisassa vasena edisaṁ tassa balaṁ daṭṭhabbaṁ.",
  "tika-s0302t8:17_12": "Uggahitasippā dhanuvedasikkhāvasena.",
  "tika-s0302t8:17_13": "Ciṇṇavasībhāvā lakkhesu avirajjhanasarakkhepavasena.",
  "tika-s0302t8:17_14": "Kataṁ rājakulādīsu upagantvā asanaṁ sarakkhepo etehīti katūpasanāti āha “rājakulādīsu dassitasippā”ti.",
  "tika-s0302t8:18_0": "“Bodhisatto cattāri kaṇḍāni āharī”ti vatvā tameva atthaṁ vitthārato dassento “tadā kirā”tiādimāha.",
  "tika-s0302t8:18_1": "Tattha javissāmāti dhāvissāma.",
  "tika-s0302t8:18_2": "Aggi uṭṭhahīti sīghapatanasantāpena ca sūriyarasmisantāpassa āsannabhāvena ca usumā uṭṭhahi.",
  "tika-s0302t8:18_3": "Pakkhapañjarenāti pakkhajālantarena.",
  "tika-s0302t8:19_0": "Nivattitvāti “nippayojanamidaṁ javana”nti nivattitvā.",
  "tika-s0302t8:19_1": "Pattakaṭāhena otthaṭapatto viyāti pihitapatto viya ahosi,",
  "tika-s0302t8:19_2": "vegasā patanena nagarassa upari ākāsassa nirikkhaṇaṁ ahosi.",
  "tika-s0302t8:19_3": "Sañcāritattā anekahaṁsasahassasadiso paññāyi seyyathāpi bodhisattassa dhanuggahakāle sarakūṭādidassane.",
  "tika-s0302t8:20_0": "Dukkaranti tassa adassanaṁ sandhāyāha,",
  "tika-s0302t8:20_1": "na attano patanaṁ.",
  "tika-s0302t8:20_2": "Sūriyamaṇḍalañhi atisīghena javena gacchantampi paññāsayojanāyāmavitthataṁ attano vipulatāya pabhassaratāya",
  "tika-s0302t8:20_3": "ca sattānaṁ cakkhussa gocarabhāvaṁ gacchati,",
  "tika-s0302t8:20_4": "javanahaṁso pana tādisena sūriyena saddhiṁ javena gacchanto na paññāyeyya.",
  "tika-s0302t8:20_5": "Tasmā vuttaṁ “na sakkā tayā passitu”nti.",
  "tika-s0302t8:20_6": "Cattāro akkhaṇavedhino.",
  "tika-s0302t8:20_7": "Gantvā gahite sotuṁ ghaṇḍaṁ piḷandhāpetvā sayaṁ puratthābhimukho nisinno.",
  "tika-s0302t8:20_8": "Puratthimadisābhimukhaṁ gatakaṇḍaṁ sandhāyāha “paṭhamakaṇḍeneva saddhiṁ uppatitvā”ti.",
  "tika-s0302t8:20_9": "Te cattāri kaṇḍāni ekakkhaṇeyeva khipiṁsu.",
  "tika-s0302t8:21_0": "Āyuṁ saṅkharoti etenāti āyusaṅkhāro.",
  "tika-s0302t8:21_1": "Yathā hi kammajarūpānaṁ pavatti jīvitindriyapaṭibaddhā,",
  "tika-s0302t8:21_2": "evaṁ attabhāvassa pavatti tappaṭibaddhāti.",
  "tika-s0302t8:21_3": "Bahuvacananiddeso pana pāḷiyaṁ ekasmiṁ khaṇe anekasatasaṅkhassa jīvitindriyassa upalabbhanato.",
  "tika-s0302t8:21_4": "Taṁ jīvitindriyaṁ.",
  "tika-s0302t8:21_5": "Tato yathāvuttadevatānaṁ javato sīghataraṁ khīyati ittarakhaṇattā.",
  "tika-s0302t8:21_6": "Vuttañhetaṁ –",
  "tika-s0302t8:22_0": "“Jīvitaṁ attabhāvo ca, sukhadukkhā ca kevalā;",
  "tika-s0302t8:23_0": "Ekacittasamāyuttā, lahuso vattate khaṇo”ti. ;",
  "tika-s0302t8:24_0": "Bhedoti bhaṅgo. Na sakkā paññāpetuṁ tatopi ativiya ittarakhaṇattā.",
  "tika-s0302t8:25_0": "Dhanuggahasuttavaṇṇanā niṭṭhitā.",
  "tika-s0302t8:26_0": "7. Āṇisuttavaṇṇanā",
  "tika-s0302t8:27_0": "229.",
  "tika-s0302t8:27_1": "Aññe rājāno ca bhāgaṁ gaṇhantā ime viya dasabhāgaṁ gaṇhantīti tesamayaṁ anugati paññāyati.",
  "tika-s0302t8:27_2": "Mahājanassa ānayanato ānakoti āha “evaṁladdhanāmo”ti.",
  "tika-s0302t8:27_3": "Idāni taṁ ādito paṭṭhāya āgamanānukkamaṁ dassetuṁ “himavante kirā”tiādimāha.",
  "tika-s0302t8:27_4": "Kareṇunti kareṇukaṁ,",
  "tika-s0302t8:27_5": "hatthininti attho.",
  "tika-s0302t8:27_6": "Sakkariṁsūti anatthapariharaṇavasena pasatthūpahāravasena ca pūjesuṁ.",
  "tika-s0302t8:27_7": "Otarīti kuḷīradahaṁ pāvisi.",
  "tika-s0302t8:27_8": "Paṭikkamitvā ṭhapanavasena apakkamitvā pati.",
  "tika-s0302t8:28_0": "Suvaṇṇarajatādimayanti kismiñci chidde suvaṇṇamayaṁ,",
  "tika-s0302t8:28_1": "kismiñci rajatamayaṁ,",
  "tika-s0302t8:28_2": "kismiñci phalikamayaṁ āṇiṁ ghaṭayiṁsu bandhiṁsu.",
  "tika-s0302t8:28_3": "Pubbe pharitvā tiṭṭhantassa dvādasa yojanāni pamāṇo etassāti dvādasayojanappamāṇo, saddo.",
  "tika-s0302t8:28_4": "Athassa anekasatakāle gacchante antosālāyampi dukkhena suyyittha āṇisaṅghāṭamattattā.",
  "tika-s0302t8:29_0": "Gambhīrāti agādhā dukkhogāḷhā.",
  "tika-s0302t8:29_1": "Sallasuttañhi “animittamanaññāta”ntiādinā pāḷivasena gambhīraṁ,",
  "tika-s0302t8:29_2": "na atthagambhīraṁ.",
  "tika-s0302t8:29_3": "Tathā hi tattha tā tā gāthā duviññeyyarūpā tiṭṭhanti,",
  "tika-s0302t8:29_4": "duviññeyyaṁ ñāṇena dukkhogāhanti katvā “gambhīra”nti vuccati.",
  "tika-s0302t8:29_5": "Pubbāparampettha kāsañci gāthānaṁ duviññeyyatāya dukkhogāhameva,",
  "tika-s0302t8:29_6": "tasmā taṁ pāḷivasena “gambhīra”nti vuttaṁ “pāḷivasena gambhīrā sallasuttasadisā”ti.",
  "tika-s0302t8:29_7": "Iminā nayena “atthavasena gambhīrā”ti ettha attho veditabbo.",
  "tika-s0302t8:29_8": "Mahāvedallasuttassa atthavasena gambhīratā pākaṭāyeva.",
  "tika-s0302t8:29_9": "Lokaṁ uttaratīti lokuttaro,",
  "tika-s0302t8:29_10": "navavidhaappamāṇadhammo,",
  "tika-s0302t8:29_11": "so atthabhūto etesaṁ atthīti lokuttarā.",
  "tika-s0302t8:29_12": "Tenāha “lokuttaraatthadīpakā”ti.",
  "tika-s0302t8:29_13": "Sattasuññatadhammamattamevāti sattena attanā suññataṁ kevalaṁ dhammamattameva.",
  "tika-s0302t8:29_14": "Uggahetabbaṁ pariyāpuṇitabbanti ca liṅgavacanavipallāsena vuttanti āha “uggahetabbe ca pariyāpuṇitabbe cā”ti.",
  "tika-s0302t8:29_15": "Kavitāti kavino kammaṁ kavikatā.",
  "tika-s0302t8:29_16": "Yassa pana yaṁ kammaṁ,",
  "tika-s0302t8:29_17": "taṁ tena katanti vuccatīti āha “kavitāti kavīhi katā”ti.",
  "tika-s0302t8:29_18": "Itaraṁ “kāveyyā”ti padaṁ,",
  "tika-s0302t8:29_19": "kabbanti vuttaṁ hoti.",
  "tika-s0302t8:29_20": "“Kabba”nti ca kavinā vuttanti attho.",
  "tika-s0302t8:29_21": "Tenāha “tasseva vevacana”nti.",
  "tika-s0302t8:29_22": "Cittakkharāti vicitrākāraakkharā.",
  "tika-s0302t8:29_23": "Sāsanato bahibhūtāti na sāsanāvacarā.",
  "tika-s0302t8:29_24": "Tesaṁ sāvakehīti “buddhānaṁ sāvakā”ti apaññātānaṁ yesaṁ kesañci sāvakehi.",
  "tika-s0302t8:29_25": "Anuggayhamānāti na uggayhamānā savanadhāraṇaparicayaatthūpaparikkhādivasena anuggayhamānā.",
  "tika-s0302t8:29_26": "Antaradhāyanti adassanaṁ gacchanti.",
  "tika-s0302t8:30_0": "Āṇisuttavaṇṇanā niṭṭhitā.",
  "tika-s0302t8:31_0": "8. Kaliṅgarasuttavaṇṇanā",
  "tika-s0302t8:32_0": "230.",
  "tika-s0302t8:32_1": "Kaliṅgaraṁ vuccati khuddakadārukhaṇḍaṁ,",
  "tika-s0302t8:32_2": "taṁ upadhānaṁ etesanti kaliṅgarūpadhānā.",
  "tika-s0302t8:32_3": "Licchavī pana khadiradaṇḍaṁ upadhānaṁ katvā tadā vihariṁsu.",
  "tika-s0302t8:32_4": "Tasmā vuttaṁ “khadiraghaṭikāsū”tiādi.",
  "tika-s0302t8:32_5": "Pakativijjusaññito natthi etesaṁ khaṇo vijjhaneti akkhaṇavedhino tato sīghataraṁ vijjhanato.",
  "tika-s0302t8:32_6": "“Akkhaṇa”nti vijju vuccati ittarakhaṇattā.",
  "tika-s0302t8:32_7": "Akkhaṇobhāsena lakkhaṇavedhakā akkhaṇavedhino.",
  "tika-s0302t8:32_8": "Anekadhā bhinnassa vālassa vijjhanena vālavedhino.",
  "tika-s0302t8:32_9": "Vālekadeso hi idha “vālo”ti gahito.",
  "tika-s0302t8:33_0": "Bahudeva divasabhāgaṁ padhānānuyogato uppannadarathaparissamavinodanatthaṁ nhāyitvā.",
  "tika-s0302t8:33_1": "Te sandhāyāti te tathārūpe padhānakammikabhikkhū sandhāya.",
  "tika-s0302t8:33_2": "Idaṁ idāni vuccamānaṁ atthajātaṁ vuttaṁ porāṇaṭṭhakathāyaṁ.",
  "tika-s0302t8:33_3": "Ayampi dīpoti tambapaṇṇidīpamāha.",
  "tika-s0302t8:33_4": "Padhānānuyuñjanavelāya nivedanavasena tattha tattha ekajjhaṁ pahataghaṇḍinigghoseneva ekaghaṇḍinigghoso,",
  "tika-s0302t8:33_5": "tattha tattha paṇṇasālādīsu vasantānaṁ bhikkhūnaṁ vasena ekapadhānabhūto.",
  "tika-s0302t8:33_6": "Nānāmukhoti anurādhapurassa pacchimadisāyaṁ eko vihāro,",
  "tika-s0302t8:33_7": "pilicchikoḷinagarassa puratthimadisāyaṁ.",
  "tika-s0302t8:33_8": "Ubhayattha pavattaghaṇḍisaddā antarāpavattaghaṇḍisaddehi missetvā osaranti.",
  "tika-s0302t8:33_9": "Kalyāṇiyaṁ pavattaghaṇḍisaddo tathā nāgadīpe.",
  "tika-s0302t8:34_0": "Kaliṅgarasuttavaṇṇanā niṭṭhitā.",
  "tika-s0302t8:35_0": "9. Nāgasuttavaṇṇanā",
  "tika-s0302t8:36_0": "231.Atikkantavelanti bhattānumodanaupanisinnakathāvelato atikkantavelaṁ.",
  "tika-s0302t8:36_1": "Asambhinnenāti sarasampattito asambhinnena,",
  "tika-s0302t8:36_2": "sarassa uccāraṇasampattiṁ aparihāpetvāti attho.",
  "tika-s0302t8:36_3": "Aparisuddhāsayatāya neva guṇavaṇṇāya na ñāṇabalāya hoti.",
  "tika-s0302t8:36_4": "Tanti taṁ tathā paccayānaṁ paribhuñjanaṁ,",
  "tika-s0302t8:36_5": "taṁ tathā micchāpaṭipajjanaṁ.",
  "tika-s0302t8:37_0": "Nāgasuttavaṇṇanā niṭṭhitā.",
  "tika-s0302t8:38_0": "10. Biḷārasuttavaṇṇanā",
  "tika-s0302t8:39_0": "232.Gharānaṁsandhīti gharena gharassa sambandhaṭṭhānaṁ.",
  "tika-s0302t8:39_1": "Saha malena vattatīti samalaṁ.",
  "tika-s0302t8:39_2": "Gehato gāmato ca nikkhamanacandanikaṭṭhānaṁ.",
  "tika-s0302t8:39_3": "Saṅkāraṭṭhānanti saṅkārakūṭaṁ.",
  "tika-s0302t8:39_4": "Keci “sandhisaṅkārakūṭaṭṭhāna”nti vadanti.",
  "tika-s0302t8:39_5": "Vuṭṭhānanti āpannaāpattito,",
  "tika-s0302t8:39_6": "na kilesato vuṭṭhānaṁ,",
  "tika-s0302t8:39_7": "suddhante adhiṭṭhānaṁ.",
  "tika-s0302t8:39_8": "Taṁ pana yathāāpannāya āpattiyā “desanā”tveva vuccatīti āha “desanā paññāyatī”ti.",
  "tika-s0302t8:40_0": "Biḷārasuttavaṇṇanā niṭṭhitā.",
  "tika-s0302t8:41_0": "11. Siṅgālasuttavaṇṇanā",
  "tika-s0302t8:42_0": "233.Ettakampīti iminā jarasiṅgālena laddhabbaṁ cittassādamattampi na labhissati sakalameva kappaṁ sabbaso",
  "tika-s0302t8:42_1": "avīcijālāhi nirantaraṁ jhāyamānatāya niccadukkhāturabhāvato.",
  "tika-s0302t8:43_0": "Siṅgālasuttavaṇṇanā niṭṭhitā.",
  "tika-s0302t8:44_0": "12. Dutiyasiṅgālasuttavaṇṇanā",
  "tika-s0302t8:45_0": "234.Katajānananti katūpakārajānanaṁ.",
  "tika-s0302t8:45_1": "Kataveditāti tasseva paresaṁ pākaṭakaraṇavasena jānanameva.",
  "tika-s0302t8:45_2": "Ācāramevāti katāparādhameva.",
  "tika-s0302t8:46_0": "Dutiyasiṅgālasuttavaṇṇanā niṭṭhitā.",
  "tika-s0302t8:47_0": "Sāratthappakāsiniyā saṁyuttanikāya-aṭṭhakathāya",
  "tika-s0302t8:48_0": "Opammasaṁyuttavaṇṇanāya līnatthappakāsanā samattā."
}