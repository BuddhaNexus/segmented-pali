{
  "ja296:0.1_0": "Jātaka",
  "ja296:0.2_0": "Tikanipāta",
  "ja296:0.3_0": "Kumbhavagga",
  "ja296:0.4_0": "6. Samuddajātaka",
  "ja296:1.1_0": "“Ko nāyaṁ loṇatoyasmiṁ,",
  "ja296:1.2_0": "samantā paridhāvati;",
  "ja296:1.3_0": "Macche makare ca vāreti,",
  "ja296:1.4_0": "ūmīsu ca vihaññati”.",
  "ja296:2.1_0": "“Anantapāyī sakuṇo,",
  "ja296:2.2_0": "atittoti disāsuto;",
  "ja296:2.3_0": "Samuddaṁ pātumicchāmi,",
  "ja296:2.4_0": "sāgaraṁ saritaṁ patiṁ”.",
  "ja296:3.1_0": "“So ayaṁ hāyati ceva,",
  "ja296:3.2_0": "pūrate ca mahodadhi;",
  "ja296:3.3_0": "Nāssa nāyati pītanto,",
  "ja296:3.4_0": "apeyyo kira sāgaro”ti.",
  "ja296:4.1_0": "Samuddajātakaṁ chaṭṭhaṁ."
}