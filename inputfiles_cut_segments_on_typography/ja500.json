{
  "ja500:0.1_0": "Jātaka",
  "ja500:0.2_0": "Vīsatinipāta",
  "ja500:0.3_0": "Mātaṅgavagga",
  "ja500:0.4_0": "4. Sirīmantajātaka",
  "ja500:1.1_0": "“Paññāyupetaṁ siriyā vihīnaṁ,",
  "ja500:1.2_0": "Yasassinaṁ vāpi apetapaññaṁ;",
  "ja500:1.3_0": "Pucchāmi taṁ senaka etamatthaṁ,",
  "ja500:1.4_0": "Kamettha seyyo kusalā vadanti”.",
  "ja500:2.1_0": "“Dhīrā ca bālā ca have janinda,",
  "ja500:2.2_0": "Sippūpapannā ca asippino ca;",
  "ja500:2.3_0": "Sujātimantopi ajātimassa,",
  "ja500:2.4_0": "Yasassino pesakarā bhavanti;",
  "ja500:2.5_0": "Etampi disvāna ahaṁ vadāmi,",
  "ja500:2.6_0": "Pañño nihīno sirīmāva seyyo”.",
  "ja500:3.1_0": "“Tuvampi pucchāmi anomapañña,",
  "ja500:3.2_0": "Mahosadha kevaladhammadassi;",
  "ja500:3.3_0": "Bālaṁ yasassiṁ paṇḍitaṁ appabhogaṁ,",
  "ja500:3.4_0": "Kamettha seyyo kusalā vadanti”.",
  "ja500:4.1_0": "“Pāpāni kammāni karoti bālo,",
  "ja500:4.2_0": "Idhameva seyyo iti maññamāno;",
  "ja500:4.3_0": "Idhalokadassī paralokamadassī,",
  "ja500:4.4_0": "Ubhayattha bālo kalimaggahesi;",
  "ja500:4.5_0": "Etampi disvāna ahaṁ vadāmi,",
  "ja500:4.6_0": "Paññova seyyo na yasassi bālo”.",
  "ja500:5.1_0": "“Na sippametaṁ vidadhāti bhogaṁ,",
  "ja500:5.2_0": "Na bandhuvā na sarīravaṇṇo yo;",
  "ja500:5.3_0": "Passeḷamūgaṁ sukhamedhamānaṁ,",
  "ja500:5.4_0": "Sirī hi naṁ bhajate goravindaṁ;",
  "ja500:5.5_0": "Etampi disvāna ahaṁ vadāmi,",
  "ja500:5.6_0": "Pañño nihīno sirīmāva seyyo”.",
  "ja500:6.1_0": "“Laddhā sukhaṁ majjati appapañño,",
  "ja500:6.2_0": "Dukkhena phuṭṭhopi pamohameti;",
  "ja500:6.3_0": "Āgantunā dukkhasukhena phuṭṭho,",
  "ja500:6.4_0": "Pavedhati vāricarova ghamme;",
  "ja500:6.5_0": "Etampi disvāna ahaṁ vadāmi,",
  "ja500:6.6_0": "Paññova seyyo na yasassi bālo”.",
  "ja500:7.1_0": "“Dumaṁ yathā sāduphalaṁ araññe,",
  "ja500:7.2_0": "Samantato samabhisaranti pakkhī;",
  "ja500:7.3_0": "Evampi aḍḍhaṁ sadhanaṁ sabhogaṁ,",
  "ja500:7.4_0": "Bahujjano bhajati atthahetu;",
  "ja500:7.5_0": "Etampi disvāna ahaṁ vadāmi,",
  "ja500:7.6_0": "Pañño nihīno sirīmāva seyyo”.",
  "ja500:8.1_0": "“Na sādhu balavā bālo,",
  "ja500:8.2_0": "Sāhasā vindate dhanaṁ;",
  "ja500:8.3_0": "Kandantametaṁ dummedhaṁ,",
  "ja500:8.4_0": "Kaḍḍhanti nirayaṁ bhusaṁ;",
  "ja500:8.5_0": "Etampi disvāna ahaṁ vadāmi,",
  "ja500:8.6_0": "Paññova seyyo na yasassi bālo”.",
  "ja500:9.1_0": "“Yā kāci najjo gaṅgamabhissavanti,",
  "ja500:9.2_0": "Sabbāva tā nāmagottaṁ jahanti;",
  "ja500:9.3_0": "Gaṅgā samuddaṁ paṭipajjamānā,",
  "ja500:9.4_0": "Na khāyate iddhiṁ paññopi loke;",
  "ja500:9.5_0": "Etampi disvāna ahaṁ vadāmi,",
  "ja500:9.6_0": "Pañño nihīno sirīmāva seyyo”.",
  "ja500:10.1_0": "“Yametamakkhā udadhiṁ mahantaṁ,",
  "ja500:10.2_0": "Savanti najjo sabbakālamasaṅkhyaṁ;",
  "ja500:10.3_0": "So sāgaro niccamuḷāravego,",
  "ja500:10.4_0": "Velaṁ na acceti mahāsamuddo.",
  "ja500:11.1_0": "Evampi bālassa pajappitāni,",
  "ja500:11.2_0": "Paññaṁ na acceti sirī kadāci;",
  "ja500:11.3_0": "Etampi disvāna ahaṁ vadāmi,",
  "ja500:11.4_0": "Paññova seyyo na yasassi bālo”.",
  "ja500:12.1_0": "“Asaññato cepi paresamatthaṁ,",
  "ja500:12.2_0": "Bhaṇāti sandhānagato yasassī;",
  "ja500:12.3_0": "Tasseva taṁ rūhati ñātimajjhe,",
  "ja500:12.4_0": "Sirī hi naṁ kārayate na paññā;",
  "ja500:12.5_0": "Etampi disvāna ahaṁ vadāmi,",
  "ja500:12.6_0": "Pañño nihīno sirīmāva seyyo”.",
  "ja500:13.1_0": "“Parassa vā attano vāpi hetu,",
  "ja500:13.2_0": "Bālo musā bhāsati appapañño;",
  "ja500:13.3_0": "So nindito hoti sabhāya majjhe,",
  "ja500:13.4_0": "Pacchāpi so duggatigāmī hoti;",
  "ja500:13.5_0": "Etampi disvāna ahaṁ vadāmi,",
  "ja500:13.6_0": "Paññova seyyo na yasassi bālo”.",
  "ja500:14.1_0": "“Atthampi ce bhāsati bhūripañño,",
  "ja500:14.2_0": "Anāḷhiyo appadhano daliddo;",
  "ja500:14.3_0": "Na tassa taṁ rūhati ñātimajjhe,",
  "ja500:14.4_0": "Sirī ca paññāṇavato na hoti;",
  "ja500:14.5_0": "Etampi disvāna ahaṁ vadāmi,",
  "ja500:14.6_0": "Pañño nihīno sirīmāva seyyo”.",
  "ja500:15.1_0": "“Parassa vā attano vāpi hetu,",
  "ja500:15.2_0": "Na bhāsati alikaṁ bhūripañño;",
  "ja500:15.3_0": "So pūjito hoti sabhāya majjhe,",
  "ja500:15.4_0": "Pacchāpi so suggatigāmī hoti;",
  "ja500:15.5_0": "Etampi disvāna ahaṁ vadāmi,",
  "ja500:15.6_0": "Paññova seyyo na yasassi bālo”.",
  "ja500:16.1_0": "“Hatthī gavassā maṇikuṇḍalā ca,",
  "ja500:16.2_0": "Thiyo ca iddhesu kulesu jātā;",
  "ja500:16.3_0": "Sabbāva tā upabhogā bhavanti,",
  "ja500:16.4_0": "Iddhassa posassa aniddhimanto;",
  "ja500:16.5_0": "Etampi disvāna ahaṁ vadāmi,",
  "ja500:16.6_0": "Pañño nihīno sirīmāva seyyo”.",
  "ja500:17.1_0": "“Asaṁvihitakammantaṁ,",
  "ja500:17.2_0": "Bālaṁ dummedhamantinaṁ;",
  "ja500:17.3_0": "Sirī jahati dummedhaṁ,",
  "ja500:17.4_0": "Jiṇṇaṁva urago tacaṁ;",
  "ja500:17.5_0": "Etampi disvāna ahaṁ vadāmi,",
  "ja500:17.6_0": "Paññova seyyo na yasassi bālo”.",
  "ja500:18.1_0": "“Pañca paṇḍitā mayaṁ bhaddante,",
  "ja500:18.2_0": "Sabbe pañjalikā upaṭṭhitā;",
  "ja500:18.3_0": "Tvaṁ no abhibhuyya issarosi,",
  "ja500:18.4_0": "Sakkova bhūtapati devarājā;",
  "ja500:18.5_0": "Etampi disvāna ahaṁ vadāmi,",
  "ja500:18.6_0": "Pañño nihīno sirīmāva seyyo”.",
  "ja500:19.1_0": "“Dāsova paññassa yasassi bālo,",
  "ja500:19.2_0": "Atthesu jātesu tathāvidhesu;",
  "ja500:19.3_0": "Yaṁ paṇḍito nipuṇaṁ saṁvidheti,",
  "ja500:19.4_0": "Sammohamāpajjati tattha bālo;",
  "ja500:19.5_0": "Etampi disvāna ahaṁ vadāmi,",
  "ja500:19.6_0": "Paññova seyyo na yasassi bālo.",
  "ja500:20.1_0": "Addhā hi paññāva sataṁ pasatthā,",
  "ja500:20.2_0": "Kantā sirī bhogaratā manussā;",
  "ja500:20.3_0": "Ñāṇañca buddhānamatulyarūpaṁ,",
  "ja500:20.4_0": "Paññaṁ na acceti sirī kadāci”.",
  "ja500:21.1_0": "“Yaṁ taṁ apucchimha akittayī no,",
  "ja500:21.2_0": "Mahosadha kevaladhammadassī;",
  "ja500:21.3_0": "Gavaṁ sahassaṁ usabhañca nāgaṁ,",
  "ja500:21.4_0": "Ājaññayutte ca rathe dasa ime;",
  "ja500:21.5_0": "Pañhassa veyyākaraṇena tuṭṭho,",
  "ja500:21.6_0": "Dadāmi te gāmavarāni soḷasā”ti.",
  "ja500:22.1_0": "Sirīmantajātakaṁ catutthaṁ."
}