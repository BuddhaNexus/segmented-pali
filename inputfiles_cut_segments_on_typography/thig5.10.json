{
  "thig5.10:0.1_0": "Therīgāthā 5.10",
  "thig5.10:0.2_0": "Pañcakanipāta",
  "thig5.10:0.3_0": "Paṭācārātherīgāthā",
  "thig5.10:1.1_0": "“Naṅgalehi kasaṁ khettaṁ,",
  "thig5.10:1.2_0": "bījāni pavapaṁ chamā;",
  "thig5.10:1.3_0": "Puttadārāni posentā,",
  "thig5.10:1.4_0": "dhanaṁ vindanti māṇavā.",
  "thig5.10:2.1_0": "Kimahaṁ sīlasampannā,",
  "thig5.10:2.2_0": "satthusāsanakārikā;",
  "thig5.10:2.3_0": "Nibbānaṁ nādhigacchāmi,",
  "thig5.10:2.4_0": "akusītā anuddhatā.",
  "thig5.10:3.1_0": "Pāde pakkhālayitvāna,",
  "thig5.10:3.2_0": "udakesu karomahaṁ;",
  "thig5.10:3.3_0": "Pādodakañca disvāna,",
  "thig5.10:3.4_0": "thalato ninnamāgataṁ.",
  "thig5.10:4.1_0": "Tato cittaṁ samādhesiṁ,",
  "thig5.10:4.2_0": "assaṁ bhadraṁvajāniyaṁ;",
  "thig5.10:4.3_0": "Tato dīpaṁ gahetvāna,",
  "thig5.10:4.4_0": "vihāraṁ pāvisiṁ ahaṁ;",
  "thig5.10:4.5_0": "Seyyaṁ olokayitvāna,",
  "thig5.10:4.6_0": "mañcakamhi upāvisiṁ.",
  "thig5.10:5.1_0": "Tato sūciṁ gahetvāna,",
  "thig5.10:5.2_0": "vaṭṭiṁ okassayāmahaṁ;",
  "thig5.10:5.3_0": "Padīpasseva nibbānaṁ,",
  "thig5.10:5.4_0": "vimokkho ahu cetaso”ti.",
  "thig5.10:6.1_0": "… Paṭācārā therī …."
}