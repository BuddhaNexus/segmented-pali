{
  "patthana22.19:0.1_0": "Paṭṭhānapakaraṇa",
  "patthana22.19:0.2_0": "Dhammapaccanīyānuloma (4), Tikadukapaṭṭhānapāḷi (4)",
  "patthana22.19:0.3_0": "22.19. Upādinnattika, Hetuduka",
  "patthana22.19:1.1_0": "Naupādinnupādāniyaṁ nahetuṁ dhammaṁ paṭicca anupādinnupādāniyo hetu dhammo uppajjati hetupaccayā.",
  "patthana22.19:1.1_1": "(Saṅkhittaṁ.) Hetuyā nava,",
  "patthana22.19:1.1_2": "ārammaṇe nava,",
  "patthana22.19:1.1_3": "adhipatiyā cha …pe…",
  "patthana22.19:1.1_4": "āsevane cha …pe…",
  "patthana22.19:1.1_5": "vipāke cha …pe…",
  "patthana22.19:1.1_6": "avigate nava.",
  "patthana22.19:2.1_0": "Naupādinnupādāniyaṁ nanahetuṁ dhammaṁ paṭicca anupādinnupādāniyo nahetu dhammo uppajjati hetupaccayā.",
  "patthana22.19:2.1_1": "(Saṅkhittaṁ.) Hetuyā aṭṭhārasa,",
  "patthana22.19:2.1_2": "ārammaṇe nava,",
  "patthana22.19:2.1_3": "adhipatiyā ekādasa …pe…",
  "patthana22.19:2.1_4": "āsevane cha …pe…",
  "patthana22.19:2.1_5": "vipāke pannarasa …pe…",
  "patthana22.19:2.1_6": "avigate aṭṭhārasa."
}