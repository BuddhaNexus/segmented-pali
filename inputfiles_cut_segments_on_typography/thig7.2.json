{
  "thig7.2:0.1_0": "Therīgāthā 7.2",
  "thig7.2:0.2_0": "Sattakanipāta",
  "thig7.2:0.3_0": "Cālātherīgāthā",
  "thig7.2:1.1_0": "“Satiṁ upaṭṭhapetvāna,",
  "thig7.2:1.2_0": "bhikkhunī bhāvitindriyā;",
  "thig7.2:1.3_0": "Paṭivijjhi padaṁ santaṁ,",
  "thig7.2:1.4_0": "saṅkhārūpasamaṁ sukhaṁ”.",
  "thig7.2:2.1_0": "“Kaṁ nu uddissa muṇḍāsi,",
  "thig7.2:2.2_0": "samaṇī viya dissasi;",
  "thig7.2:2.3_0": "Na ca rocesi pāsaṇḍe,",
  "thig7.2:2.4_0": "kimidaṁ carasi momuhā”.",
  "thig7.2:3.1_0": "“Ito bahiddhā pāsaṇḍā,",
  "thig7.2:3.2_0": "diṭṭhiyo upanissitā;",
  "thig7.2:3.3_0": "Na te dhammaṁ vijānanti,",
  "thig7.2:3.4_0": "na te dhammassa kovidā.",
  "thig7.2:4.1_0": "Atthi sakyakule jāto,",
  "thig7.2:4.2_0": "buddho appaṭipuggalo;",
  "thig7.2:4.3_0": "So me dhammamadesesi,",
  "thig7.2:4.4_0": "diṭṭhīnaṁ samatikkamaṁ.",
  "thig7.2:5.1_0": "Dukkhaṁ dukkhasamuppādaṁ,",
  "thig7.2:5.2_0": "Dukkhassa ca atikkamaṁ;",
  "thig7.2:5.3_0": "Ariyaṁ caṭṭhaṅgikaṁ maggaṁ,",
  "thig7.2:5.4_0": "Dukkhūpasamagāminaṁ.",
  "thig7.2:6.1_0": "Tassāhaṁ vacanaṁ sutvā,",
  "thig7.2:6.2_0": "vihariṁ sāsane ratā;",
  "thig7.2:6.3_0": "Tisso vijjā anuppattā,",
  "thig7.2:6.4_0": "kataṁ buddhassa sāsanaṁ.",
  "thig7.2:7.1_0": "Sabbattha vihatā nandī,",
  "thig7.2:7.2_0": "tamokhandho padālito;",
  "thig7.2:7.3_0": "Evaṁ jānāhi pāpima,",
  "thig7.2:7.4_0": "nihato tvamasi antaka”.",
  "thig7.2:8.1_0": "… Cālā therī …."
}