{
  "tha-ap211:0.1_0": "Therāpadāna",
  "tha-ap211:0.2_0": "Kaṇikārapupphiyavagga",
  "tha-ap211:0.3_0": "9. Ādhāradāyakattheraapadāna",
  "tha-ap211:1.1_0": "“Ādhārakaṁ mayā dinnaṁ,",
  "tha-ap211:1.2_0": "sikhino lokabandhuno;",
  "tha-ap211:1.3_0": "Dhāremi pathaviṁ sabbaṁ,",
  "tha-ap211:1.4_0": "kevalaṁ vasudhaṁ imaṁ.",
  "tha-ap211:2.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap211:2.2_0": "bhavā sabbe samūhatā;",
  "tha-ap211:2.3_0": "Dhāremi antimaṁ dehaṁ,",
  "tha-ap211:2.4_0": "sammāsambuddhasāsane.",
  "tha-ap211:3.1_0": "Sattavīse ito kappe,",
  "tha-ap211:3.2_0": "ahesuṁ caturo janā;",
  "tha-ap211:3.3_0": "Samantavaraṇā nāma,",
  "tha-ap211:3.4_0": "cakkavattī mahabbalā.",
  "tha-ap211:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap211:4.2_0": "…pe…",
  "tha-ap211:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap211:5.1_0": "Itthaṁ sudaṁ āyasmā ādhāradāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap211:6.1_0": "Ādhāradāyakattherassāpadānaṁ navamaṁ."
}