{
  "thag2.10:0.1_0": "Theragāthā 2.10",
  "thag2.10:0.2_0": "Dukanipāta",
  "thag2.10:0.3_0": "Paṭhamavagga",
  "thag2.10:0.4_0": "Vasabhattheragāthā",
  "thag2.10:1.1_0": "“Pubbe hanati attānaṁ,",
  "thag2.10:1.2_0": "pacchā hanati so pare;",
  "thag2.10:1.3_0": "Suhataṁ hanti attānaṁ,",
  "thag2.10:1.4_0": "vītaṁseneva pakkhimā.",
  "thag2.10:2.1_0": "Na brāhmaṇo bahivaṇṇo,",
  "thag2.10:2.2_0": "anto vaṇṇo hi brāhmaṇo;",
  "thag2.10:2.3_0": "Yasmiṁ pāpāni kammāni,",
  "thag2.10:2.4_0": "sa ve kaṇho sujampatī”ti.",
  "thag2.10:2.5_0": "… Vasabho thero …",
  "thag2.10:2.6_0": "vaggo paṭhamo.",
  "thag2.10:3.0_0": "Tassuddānaṁ",
  "thag2.10:3.1_0": "Uttaro ceva piṇḍolo,",
  "thag2.10:3.2_0": "valliyo tīriyo isi;",
  "thag2.10:3.3_0": "Ajino ca meḷajino,",
  "thag2.10:3.4_0": "rādho surādho gotamo;",
  "thag2.10:3.5_0": "Vasabhena ime honti,",
  "thag2.10:3.6_0": "dasa therā mahiddhikāti."
}