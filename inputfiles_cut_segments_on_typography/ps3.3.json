{
  "ps3.3:0.1_0": "Paṭisambhidāmagga",
  "ps3.3:0.2_0": "3 Paññāvagga",
  "ps3.3:0.3_0": "3.3. Abhisamayakathā",
  "ps3.3:1.1_0": "Abhisamayoti.",
  "ps3.3:1.2_0": "Kena abhisameti?",
  "ps3.3:1.3_0": "Cittena abhisameti.",
  "ps3.3:2.1_0": "Hañci cittena abhisameti, tena hi aññāṇī abhisameti?",
  "ps3.3:2.2_0": "Na aññāṇī abhisameti.",
  "ps3.3:2.3_0": "Ñāṇena abhisameti.",
  "ps3.3:3.1_0": "Hañci ñāṇena abhisameti, tena hi acittena ca ñāṇena ca acittako abhisameti?",
  "ps3.3:3.2_0": "Na acittako abhisameti.",
  "ps3.3:3.3_0": "Cittena ca ñāṇena ca abhisameti.",
  "ps3.3:4.1_0": "Hañci cittena ca ñāṇena ca abhisameti, tena hi kāmāvacaracittena ca ñāṇena ca abhisameti?",
  "ps3.3:4.2_0": "Na kāmāvacaracittena ca ñāṇena ca abhisameti.",
  "ps3.3:5.1_0": "Tena hi rūpāvacaracittena ca ñāṇena ca abhisameti?",
  "ps3.3:5.2_0": "Na rūpāvacaracittena ca ñāṇena ca abhisameti.",
  "ps3.3:6.1_0": "Tena hi arūpāvacaracittena ca ñāṇena ca abhisameti?",
  "ps3.3:6.2_0": "Na arūpāvacaracittena ca ñāṇena ca abhisameti.",
  "ps3.3:7.1_0": "Tena hi kammassakatacittena ca ñāṇena ca abhisameti?",
  "ps3.3:7.2_0": "Na kammassakatacittena ca ñāṇena ca abhisameti.",
  "ps3.3:8.1_0": "Tena hi saccānulomikacittena ca ñāṇena ca abhisameti?",
  "ps3.3:8.2_0": "Na saccānulomikacittena ca ñāṇena ca abhisameti.",
  "ps3.3:9.1_0": "Tena hi atītacittena ca ñāṇena ca abhisameti?",
  "ps3.3:9.2_0": "Na atītacittena ca ñāṇena ca abhisameti.",
  "ps3.3:10.1_0": "Tena hi anāgatacittena ca ñāṇena ca abhisameti?",
  "ps3.3:10.2_0": "Na anāgatacittena ca ñāṇena ca abhisameti.",
  "ps3.3:11.1_0": "Tena hi paccuppannalokiyacittena ca ñāṇena ca abhisameti?",
  "ps3.3:11.2_0": "Na paccuppannalokiyacittena ca ñāṇena ca abhisameti.",
  "ps3.3:11.3_0": "Lokuttaramaggakkhaṇe paccuppannacittena ca ñāṇena ca abhisameti.",
  "ps3.3:12.1_0": "Kathaṁ lokuttaramaggakkhaṇe paccuppannacittena ca ñāṇena ca abhisameti?",
  "ps3.3:12.2_0": "Lokuttaramaggakkhaṇe uppādādhipateyyaṁ cittaṁ ñāṇassa hetu paccayo ca taṁsampayuttaṁ nirodhagocaraṁ dassanādhipateyyaṁ",
  "ps3.3:12.2_1": "ñāṇaṁ cittassa hetu paccayo ca taṁsampayuttaṁ ñāṇaṁ nirodhagocaraṁ.",
  "ps3.3:12.3_0": "Evaṁ lokuttaramaggakkhaṇe paccuppannacittena ca ñāṇena ca abhisameti.",
  "ps3.3:13.1_0": "Kiṁ nu ettakoyeva abhisamayoti?",
  "ps3.3:13.2_0": "Na hi.",
  "ps3.3:13.3_0": "Lokuttaramaggakkhaṇe dassanābhisamayo sammādiṭṭhi,",
  "ps3.3:13.3_1": "abhiniropanābhisamayo sammāsaṅkappo,",
  "ps3.3:13.3_2": "pariggahābhisamayo sammāvācā,",
  "ps3.3:13.3_3": "samuṭṭhānābhisamayo sammākammanto,",
  "ps3.3:13.3_4": "vodānābhisamayo sammāājīvo,",
  "ps3.3:13.3_5": "paggahābhisamayo sammāvāyāmo,",
  "ps3.3:13.3_6": "upaṭṭhānābhisamayo sammāsati,",
  "ps3.3:13.3_7": "avikkhepābhisamayo sammāsamādhi;",
  "ps3.3:13.4_0": "upaṭṭhānābhisamayo satisambojjhaṅgo,",
  "ps3.3:13.4_1": "pavicayābhisamayo dhammavicayasambojjhaṅgo,",
  "ps3.3:13.4_2": "paggahābhisamayo vīriyasambojjhaṅgo,",
  "ps3.3:13.4_3": "pharaṇābhisamayo pītisambojjhaṅgo,",
  "ps3.3:13.4_4": "upasamābhisamayo passaddhisambojjhaṅgo,",
  "ps3.3:13.4_5": "avikkhepābhisamayo samādhisambojjhaṅgo,",
  "ps3.3:13.4_6": "paṭisaṅkhānābhisamayo upekkhāsambojjhaṅgo;",
  "ps3.3:13.5_0": "assaddhiye akampiyābhisamayo saddhābalaṁ,",
  "ps3.3:13.5_1": "kosajje akampiyābhisamayo vīriyabalaṁ,",
  "ps3.3:13.5_2": "pamāde akampiyābhisamayo satibalaṁ,",
  "ps3.3:13.5_3": "uddhacce akampiyābhisamayo samādhibalaṁ,",
  "ps3.3:13.5_4": "avijjāya akampiyābhisamayo paññābalaṁ;",
  "ps3.3:13.6_0": "adhimokkhābhisamayo saddhindriyaṁ,",
  "ps3.3:13.6_1": "paggahābhisamayo vīriyindriyaṁ,",
  "ps3.3:13.6_2": "upaṭṭhānābhisamayo satindriyaṁ,",
  "ps3.3:13.6_3": "avikkhepābhisamayo samādhindriyaṁ,",
  "ps3.3:13.6_4": "dassanābhisamayo paññindriyaṁ.",
  "ps3.3:13.7_0": "Ādhipateyyaṭṭhena indriyābhisamayo,",
  "ps3.3:13.7_1": "akampiyaṭṭhena balābhisamayo,",
  "ps3.3:13.7_2": "niyyānaṭṭhena bojjhaṅgābhisamayo,",
  "ps3.3:13.7_3": "hetuṭṭhena maggābhisamayo,",
  "ps3.3:13.7_4": "upaṭṭhānaṭṭhena satipaṭṭhānābhisamayo,",
  "ps3.3:13.7_5": "padahaṭṭhena sammappadhānābhisamayo,",
  "ps3.3:13.7_6": "ijjhanaṭṭhena iddhipādābhisamayo,",
  "ps3.3:13.7_7": "tathaṭṭhena saccābhisamayo,",
  "ps3.3:13.7_8": "avikkhepaṭṭhena samathābhisamayo,",
  "ps3.3:13.7_9": "anupassanaṭṭhena vipassanābhisamayo,",
  "ps3.3:13.7_10": "ekarasaṭṭhena samathavipassanābhisamayo,",
  "ps3.3:13.7_11": "anativattanaṭṭhena yuganaddhābhisamayo,",
  "ps3.3:13.7_12": "saṁvaraṭṭhena sīlavisuddhiabhisamayo,",
  "ps3.3:13.7_13": "avikkhepaṭṭhena visuddhiabhisamayo,",
  "ps3.3:13.7_14": "dassanaṭṭhena diṭṭhivisuddhiabhisamayo,",
  "ps3.3:13.7_15": "muttaṭṭhena vimokkhābhisamayo,",
  "ps3.3:13.7_16": "paṭivedhaṭṭhena vijjābhisamayo,",
  "ps3.3:13.7_17": "pariccāgaṭṭhena vimuttiabhisamayo,",
  "ps3.3:13.7_18": "samucchedaṭṭhena khaye ñāṇaṁ abhisamayo.",
  "ps3.3:13.8_0": "Chando mūlaṭṭhena abhisamayo,",
  "ps3.3:13.8_1": "manasikāro samuṭṭhānaṭṭhena abhisamayo,",
  "ps3.3:13.8_2": "phasso samodhānaṭṭhena abhisamayo,",
  "ps3.3:13.8_3": "vedanā samosaraṇaṭṭhena abhisamayo,",
  "ps3.3:13.8_4": "samādhi pamukhaṭṭhena abhisamayo,",
  "ps3.3:13.8_5": "sati ādhipateyyaṭṭhena abhisamayo,",
  "ps3.3:13.8_6": "paññā taduttaraṭṭhena abhisamayo,",
  "ps3.3:13.8_7": "vimutti sāraṭṭhena abhisamayo,",
  "ps3.3:13.8_8": "amatogadhaṁ nibbānaṁ pariyosānaṭṭhena abhisamayo.",
  "ps3.3:14.1_0": "Kiṁ nu ettakoyeva abhisamayoti?",
  "ps3.3:14.2_0": "Na hi.",
  "ps3.3:14.3_0": "Sotāpattimaggakkhaṇe dassanābhisamayo sammādiṭṭhi …pe…",
  "ps3.3:14.4_0": "amatogadhaṁ nibbānaṁ pariyosānaṭṭhena abhisamayo.",
  "ps3.3:15.1_0": "Kiṁ nu ettakoyeva abhisamayoti?",
  "ps3.3:15.2_0": "Na hi.",
  "ps3.3:15.3_0": "Sotāpattiphalakkhaṇe dassanābhisamayo sammādiṭṭhi …pe…",
  "ps3.3:15.4_0": "paṭippassaddhaṭṭhena anuppāde ñāṇaṁ abhisamayo.",
  "ps3.3:15.5_0": "Chando mūlaṭṭhena abhisamayo …pe…",
  "ps3.3:15.6_0": "amatogadhaṁ nibbānaṁ pariyosānaṭṭhena abhisamayo.",
  "ps3.3:16.1_0": "Kiṁ nu ettakoyeva abhisamayoti?",
  "ps3.3:16.2_0": "Na hi.",
  "ps3.3:16.3_0": "Sakadāgāmimaggakkhaṇe …pe…",
  "ps3.3:16.4_0": "sakadāgāmiphalakkhaṇe …",
  "ps3.3:16.5_0": "anāgāmimaggakkhaṇe …",
  "ps3.3:16.6_0": "anāgāmiphalakkhaṇe …",
  "ps3.3:16.7_0": "arahattamaggakkhaṇe …pe…",
  "ps3.3:16.8_0": "arahattaphalakkhaṇe dassanābhisamayo sammādiṭṭhi, abhiniropanābhisamayo sammāsaṅkappo …pe…",
  "ps3.3:16.9_0": "paṭippassaddhaṭṭhena anuppāde ñāṇaṁ abhisamayo.",
  "ps3.3:16.10_0": "Chando mūlaṭṭhena abhisamayo …pe…",
  "ps3.3:16.11_0": "amatogadhaṁ nibbānaṁ pariyosānaṭṭhena abhisamayo.",
  "ps3.3:17.1_0": "Yvāyaṁ kilese pajahati, atīte kilese pajahati …pe…",
  "ps3.3:17.2_0": "anāgate kilese pajahati, paccuppanne kilese pajahati, atīte kilese pajahatīti.",
  "ps3.3:17.3_0": "Hañci atīte kilese pajahati,",
  "ps3.3:17.3_1": "tena hi khīṇaṁ khepeti,",
  "ps3.3:17.3_2": "niruddhaṁ nirodheti,",
  "ps3.3:17.3_3": "vigataṁ vigameti,",
  "ps3.3:17.3_4": "atthaṅgataṁ atthaṅgameti,",
  "ps3.3:17.3_5": "atītaṁ yaṁ na atthi taṁ pajahatīti?",
  "ps3.3:17.4_0": "Na atīte kilese pajahatīti.",
  "ps3.3:17.5_0": "Anāgate kilese pajahatīti.",
  "ps3.3:17.6_0": "Hañci anāgate kilese pajahati,",
  "ps3.3:17.6_1": "tena hi ajātaṁ pajahati,",
  "ps3.3:17.6_2": "anibbattaṁ pajahati,",
  "ps3.3:17.6_3": "anuppannaṁ pajahati,",
  "ps3.3:17.6_4": "apātubhūtaṁ pajahati,",
  "ps3.3:17.6_5": "anāgataṁ yaṁ na atthi taṁ pajahatīti?",
  "ps3.3:17.7_0": "Na anāgate kilese pajahatīti.",
  "ps3.3:17.8_0": "Paccuppanne kilese pajahatīti.",
  "ps3.3:17.9_0": "Hañci paccuppanne kilese pajahati,",
  "ps3.3:17.9_1": "tena hi ratto rāgaṁ pajahati,",
  "ps3.3:17.9_2": "duṭṭho dosaṁ pajahati,",
  "ps3.3:17.9_3": "mūḷho mohaṁ pajahati,",
  "ps3.3:17.9_4": "vinibaddho mānaṁ pajahati,",
  "ps3.3:17.9_5": "parāmaṭṭho diṭṭhiṁ pajahati,",
  "ps3.3:17.9_6": "vikkhepagato uddhaccaṁ pajahati,",
  "ps3.3:17.9_7": "aniṭṭhaṅgato vicikicchaṁ pajahati,",
  "ps3.3:17.9_8": "thāmagato anusayaṁ pajahati,",
  "ps3.3:17.9_9": "kaṇhasukkadhammā yuganaddhā samameva vattanti,",
  "ps3.3:17.9_10": "saṅkilesikā maggabhāvanā hoti?",
  "ps3.3:18.1_0": "Na hi atīte kilese pajahati, na anāgate kilese pajahati, na paccuppanne kilese pajahatīti.",
  "ps3.3:18.2_0": "Hañci na atīte kilese pajahati, na anāgate …pe…",
  "ps3.3:18.3_0": "na paccuppanne kilese pajahati,",
  "ps3.3:18.3_1": "tena hi natthi maggabhāvanā,",
  "ps3.3:18.3_2": "natthi phalasacchikiriyā,",
  "ps3.3:18.3_3": "natthi kilesappahānaṁ,",
  "ps3.3:18.3_4": "natthi dhammābhisamayoti?",
  "ps3.3:18.4_0": "Atthi maggabhāvanā, atthi phalasacchikiriyā, atthi kilesappahānaṁ, atthi dhammābhisamayo.",
  "ps3.3:18.5_0": "Yathā kathaṁ viya?",
  "ps3.3:18.6_0": "Seyyathāpi taruṇo rukkho ajātaphalo.",
  "ps3.3:18.7_0": "Tamenaṁ puriso mūlaṁ chindeyya.",
  "ps3.3:18.8_0": "Ye tassa rukkhassa ajātaphalā,",
  "ps3.3:18.8_1": "te ajātāyeva na jāyanti,",
  "ps3.3:18.8_2": "anibbattāyeva na nibbattanti,",
  "ps3.3:18.8_3": "anuppannāyeva na uppajjanti,",
  "ps3.3:18.8_4": "apātubhūtāyeva na pātubhavanti.",
  "ps3.3:18.9_0": "Evamevaṁ uppādo hetu, uppādo paccayo kilesānaṁ nibbattiyāti.",
  "ps3.3:18.10_0": "Uppāde ādīnavaṁ disvā anuppāde cittaṁ pakkhandati.",
  "ps3.3:18.11_0": "Anuppāde cittassa pakkhandattā ye uppādapaccayā kilesā nibbatteyyuṁ,",
  "ps3.3:18.11_1": "te ajātāyeva na jāyanti,",
  "ps3.3:18.11_2": "anibbattāyeva na nibbattanti,",
  "ps3.3:18.11_3": "anuppannāyeva na uppajjanti,",
  "ps3.3:18.11_4": "apātubhūtāyeva na pātubhavanti.",
  "ps3.3:18.12_0": "Evaṁ hetunirodhā dukkhanirodho.",
  "ps3.3:18.13_0": "Pavattaṁ hetu, nimittaṁ hetu, āyūhanā hetu.",
  "ps3.3:18.14_0": "Āyūhanā paccayo kilesānaṁ nibbattiyāti.",
  "ps3.3:18.15_0": "Āyūhane ādīnavaṁ disvā anāyūhane cittaṁ pakkhandati.",
  "ps3.3:18.16_0": "Anāyūhane cittassa pakkhandattā,",
  "ps3.3:18.16_1": "ye āyūhanapaccayā kilesā nibbatteyyuṁ,",
  "ps3.3:18.16_2": "te ajātāyeva na jāyanti,",
  "ps3.3:18.16_3": "anibbattāyeva na nibbattanti,",
  "ps3.3:18.16_4": "anuppannāyeva na uppajjanti,",
  "ps3.3:18.16_5": "apātubhūtāyeva na pātubhavanti.",
  "ps3.3:18.17_0": "Evaṁ hetunirodhā dukkhanirodho.",
  "ps3.3:18.18_0": "Evaṁ atthi maggabhāvanā, atthi phalasacchikiriyā, atthi kilesappahānaṁ, atthi dhammābhisamayoti.",
  "ps3.3:18.19_0": "Abhisamayakathā niṭṭhitā."
}