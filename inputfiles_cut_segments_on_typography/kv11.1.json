{
  "kv11.1:0.1_0": "Kathāvatthu",
  "kv11.1:0.2_0": "Anusayapaṇṇāsaka",
  "kv11.1:0.3_0": "Ekādasamavagga",
  "kv11.1:0.4_0": "Tissopianusayakathā",
  "kv11.1:1.1_0": "Anusayā abyākatāti?",
  "kv11.1:1.2_0": "Āmantā.",
  "kv11.1:1.3_0": "Vipākābyākatā kiriyābyākatā rūpaṁ nibbānaṁ cakkhāyatanaṁ …pe… phoṭṭhabbāyatananti?",
  "kv11.1:1.4_0": "Na hevaṁ vattabbe …pe….",
  "kv11.1:2.1_0": "Kāmarāgānusayo abyākatoti?",
  "kv11.1:2.2_0": "Āmantā.",
  "kv11.1:2.3_0": "Kāmarāgo kāmarāgapariyuṭṭhānaṁ kāmarāgasaṁyojanaṁ kāmogho kāmayogo kāmacchandanīvaraṇaṁ abyākatanti?",
  "kv11.1:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv11.1:2.5_0": "kāmarāgo kāmarāgapariyuṭṭhānaṁ …pe… kāmacchandanīvaraṇaṁ akusalanti?",
  "kv11.1:2.6_0": "Āmantā.",
  "kv11.1:2.7_0": "Kāmarāgānusayo akusaloti?",
  "kv11.1:2.8_0": "Na hevaṁ vattabbe …pe….",
  "kv11.1:3.1_0": "Paṭighānusayo abyākatoti?",
  "kv11.1:3.2_0": "Āmantā.",
  "kv11.1:3.3_0": "Paṭighaṁ paṭighapariyuṭṭhānaṁ paṭighasaṁyojanaṁ abyākatanti?",
  "kv11.1:3.4_0": "Na hevaṁ vattabbe …pe…",
  "kv11.1:3.5_0": "paṭighaṁ paṭighapariyuṭṭhānaṁ paṭighasaṁyojanaṁ akusalanti?",
  "kv11.1:3.6_0": "Āmantā.",
  "kv11.1:3.7_0": "Paṭighānusayo akusaloti?",
  "kv11.1:3.8_0": "Na hevaṁ vattabbe …pe….",
  "kv11.1:4.1_0": "Mānānusayo abyākatoti?",
  "kv11.1:4.2_0": "Āmantā.",
  "kv11.1:4.3_0": "Māno mānapariyuṭṭhānaṁ mānasaṁyojanaṁ abyākatanti?",
  "kv11.1:4.4_0": "Na hevaṁ vattabbe …pe…",
  "kv11.1:4.5_0": "māno mānapariyuṭṭhānaṁ mānasaṁyojanaṁ akusalanti?",
  "kv11.1:4.6_0": "Āmantā.",
  "kv11.1:4.7_0": "Mānānusayo akusaloti?",
  "kv11.1:4.8_0": "Na hevaṁ vattabbe …pe….",
  "kv11.1:5.1_0": "Diṭṭhānusayo abyākatoti?",
  "kv11.1:5.2_0": "Āmantā.",
  "kv11.1:5.3_0": "Diṭṭhi diṭṭhogho diṭṭhiyogo diṭṭhipariyuṭṭhānaṁ diṭṭhisaṁyojanaṁ abyākatanti?",
  "kv11.1:5.4_0": "Na hevaṁ vattabbe …pe…",
  "kv11.1:5.5_0": "diṭṭhi diṭṭhogho diṭṭhiyogo diṭṭhipariyuṭṭhānaṁ diṭṭhisaṁyojanaṁ akusalanti?",
  "kv11.1:5.6_0": "Āmantā.",
  "kv11.1:5.7_0": "Diṭṭhānusayo akusaloti?",
  "kv11.1:5.8_0": "Na hevaṁ vattabbe …pe….",
  "kv11.1:6.1_0": "Vicikicchānusayo abyākatoti?",
  "kv11.1:6.2_0": "Āmantā.",
  "kv11.1:6.3_0": "Vicikicchā vicikicchāpariyuṭṭhānaṁ vicikicchāsaṁyojanaṁ vicikicchānīvaraṇaṁ abyākatanti?",
  "kv11.1:6.4_0": "Na hevaṁ vattabbe …pe…",
  "kv11.1:6.5_0": "vicikicchā vicikicchāpariyuṭṭhānaṁ vicikicchāsaṁyojanaṁ vicikicchānīvaraṇaṁ akusalanti?",
  "kv11.1:6.6_0": "Āmantā.",
  "kv11.1:6.7_0": "Vicikicchānusayo akusaloti?",
