{
  "bv9:0.1_0": "Buddhavaṁsa",
  "bv9:0.2_0": "9 Anomadassībuddhavaṁsa",
  "bv9:1.1_0": "“Sobhitassa aparena,",
  "bv9:1.2_0": "Sambuddho dvipaduttamo;",
  "bv9:1.3_0": "Anomadassī amitayaso,",
  "bv9:1.4_0": "Tejassī duratikkamo.",
  "bv9:2.1_0": "So chetvā bandhanaṁ sabbaṁ,",
  "bv9:2.2_0": "viddhaṁsetvā tayo bhave;",
  "bv9:2.3_0": "Anivattigamanaṁ maggaṁ,",
  "bv9:2.4_0": "desesi devamānuse.",
  "bv9:3.1_0": "Sāgarova asaṅkhobbho,",
  "bv9:3.2_0": "pabbatova durāsado;",
  "bv9:3.3_0": "Ākāsova ananto so,",
  "bv9:3.4_0": "sālarājāva phullito.",
  "bv9:4.1_0": "Dassanenapi taṁ buddhaṁ,",
  "bv9:4.2_0": "tositā honti pāṇino;",
  "bv9:4.3_0": "Byāharantaṁ giraṁ sutvā,",
  "bv9:4.4_0": "amataṁ pāpuṇanti te.",
  "bv9:5.1_0": "Dhammābhisamayo tassa,",
  "bv9:5.2_0": "Iddho phīto tadā ahu;",
  "bv9:5.3_0": "Koṭisatāni abhisamiṁsu,",
  "bv9:5.4_0": "Paṭhame dhammadesane.",
  "bv9:6.1_0": "Tato paraṁ abhisamaye,",
  "bv9:6.2_0": "Vassante dhammavuṭṭhiyo;",
  "bv9:6.3_0": "Asītikoṭiyobhisamiṁsu,",
  "bv9:6.4_0": "Dutiye dhammadesane.",
  "bv9:7.1_0": "Tatoparañhi vassante,",
  "bv9:7.2_0": "tappayante ca pāṇinaṁ;",
  "bv9:7.3_0": "Aṭṭhasattatikoṭīnaṁ,",
  "bv9:7.4_0": "tatiyābhisamayo ahu.",
  "bv9:8.1_0": "Sannipātā tayo āsuṁ,",
  "bv9:8.2_0": "tassāpi ca mahesino;",
  "bv9:8.3_0": "Abhiññābalappattānaṁ,",
  "bv9:8.4_0": "pupphitānaṁ vimuttiyā.",
  "bv9:9.1_0": "Aṭṭhasatasahassānaṁ,",
  "bv9:9.2_0": "sannipāto tadā ahu;",
  "bv9:9.3_0": "Pahīnamadamohānaṁ,",
  "bv9:9.4_0": "santacittāna tādinaṁ.",
  "bv9:10.1_0": "Sattasatasahassānaṁ,",
  "bv9:10.2_0": "dutiyo āsi samāgamo;",
  "bv9:10.3_0": "Anaṅgaṇānaṁ virajānaṁ,",
  "bv9:10.4_0": "upasantāna tādinaṁ.",
  "bv9:11.1_0": "Channaṁ satasahassānaṁ,",
  "bv9:11.2_0": "tatiyo āsi samāgamo;",
  "bv9:11.3_0": "Abhiññābalappattānaṁ,",
  "bv9:11.4_0": "nibbutānaṁ tapassinaṁ.",
  "bv9:12.1_0": "Ahaṁ tena samayena,",
  "bv9:12.2_0": "yakkho āsiṁ mahiddhiko;",
  "bv9:12.3_0": "Nekānaṁ yakkhakoṭīnaṁ,",
  "bv9:12.4_0": "vasavattimhi issaro.",
  "bv9:13.1_0": "Tadāpi taṁ buddhavaraṁ,",
  "bv9:13.2_0": "upagantvā mahesinaṁ;",
  "bv9:13.3_0": "Annapānena tappesiṁ,",
  "bv9:13.4_0": "sasaṅghaṁ lokanāyakaṁ.",
  "bv9:14.1_0": "Sopi maṁ tadā byākāsi,",
  "bv9:14.2_0": "visuddhanayano muni;",
  "bv9:14.3_0": "‘Aparimeyyito kappe,",
  "bv9:14.4_0": "ayaṁ buddho bhavissati.",
  "bv9:15.1_0": "Padhānaṁ padahitvāna,",
  "bv9:15.2_0": "…pe…",
  "bv9:15.3_0": "hessāma sammukhā imaṁ’.",
  "bv9:16.1_0": "Tassāpi vacanaṁ sutvā,",
  "bv9:16.2_0": "haṭṭho saṁviggamānaso;",
  "bv9:16.3_0": "Uttariṁ vatamadhiṭṭhāsiṁ,",
  "bv9:16.4_0": "dasapāramipūriyā.",
  "bv9:17.1_0": "Nagaraṁ candavatī nāma,",
  "bv9:17.2_0": "yasavā nāma khattiyo;",
  "bv9:17.3_0": "Mātā yasodharā nāma,",
  "bv9:17.4_0": "anomadassissa satthuno.",
  "bv9:18.1_0": "Dasavassasahassāni,",
  "bv9:18.2_0": "agāraṁ ajjha so vasi;",
  "bv9:18.3_0": "Sirī upasirī vaḍḍho,",
  "bv9:18.4_0": "tayo pāsādamuttamā.",
  "bv9:19.1_0": "Tevīsatisahassāni,",
  "bv9:19.2_0": "nāriyo samalaṅkatā;",
  "bv9:19.3_0": "Sirimā nāma sā nārī,",
  "bv9:19.4_0": "upavāṇo nāma atrajo.",
  "bv9:20.1_0": "Nimitte caturo disvā,",
  "bv9:20.2_0": "sivikāyābhinikkhami;",
  "bv9:20.3_0": "Anūnadasamāsāni,",
  "bv9:20.4_0": "padhānaṁ padahī jino.",
  "bv9:21.1_0": "Brahmunā yācito santo,",
  "bv9:21.2_0": "anomadassī mahāmuni;",
  "bv9:21.3_0": "Vatti cakkaṁ mahāvīro,",
  "bv9:21.4_0": "uyyāne so sudassane.",
  "bv9:22.1_0": "Nisabho ca anomo ca,",
  "bv9:22.2_0": "ahesuṁ aggasāvakā;",
  "bv9:22.3_0": "Varuṇo nāmupaṭṭhāko,",
  "bv9:22.4_0": "anomadassissa satthuno.",
  "bv9:23.1_0": "Sundarī ca sumanā ca,",
  "bv9:23.2_0": "ahesuṁ aggasāvikā;",
  "bv9:23.3_0": "Bodhi tassa bhagavato,",
  "bv9:23.4_0": "ajjunoti pavuccati.",
  "bv9:24.1_0": "Nandivaḍḍho sirivaḍḍho,",
  "bv9:24.2_0": "ahesuṁ aggupaṭṭhakā;",
  "bv9:24.3_0": "Uppalā ceva padumā ca,",
  "bv9:24.4_0": "ahesuṁ aggupaṭṭhikā.",
  "bv9:25.1_0": "Aṭṭhapaṇṇāsaratanaṁ,",
  "bv9:25.2_0": "accuggato mahāmuni;",
  "bv9:25.3_0": "Pabhā niddhāvatī tassa,",
  "bv9:25.4_0": "sataraṁsīva uggato.",
  "bv9:26.1_0": "Vassasatasahassāni,",
  "bv9:26.2_0": "āyu vijjati tāvade;",
  "bv9:26.3_0": "Tāvatā tiṭṭhamāno so,",
  "bv9:26.4_0": "tāresi janataṁ bahuṁ.",
  "bv9:27.1_0": "Supupphitaṁ pāvacanaṁ,",
  "bv9:27.2_0": "arahantehi tādihi;",
  "bv9:27.3_0": "Vītarāgehi vimalehi,",
  "bv9:27.4_0": "sobhittha jinasāsanaṁ.",
  "bv9:28.1_0": "So ca satthā amitayaso,",
  "bv9:28.2_0": "Yugāni tāni atuliyāni;",
  "bv9:28.3_0": "Sabbaṁ tamantarahitaṁ,",
  "bv9:28.4_0": "Nanu rittā sabbasaṅkhārā.",
  "bv9:29.1_0": "Anomadassī jino satthā,",
  "bv9:29.2_0": "Dhammārāmamhi nibbuto;",
  "bv9:29.3_0": "Tatthevassa jinathūpo,",
  "bv9:29.4_0": "Ubbedho pañcavīsatī”ti.",
  "bv9:30.1_0": "Anomadassissa bhagavato vaṁso sattamo."
}