{
  "ja158:0.1_0": "Jātaka",
  "ja158:0.2_0": "Dukanipāta",
  "ja158:0.3_0": "Daḷhavagga",
  "ja158:0.4_0": "8. Suhanujātaka",
  "ja158:1.1_0": "“Nayidaṁ visamasīlena,",
  "ja158:1.2_0": "soṇena suhanū saha;",
  "ja158:1.3_0": "Suhanūpi tādisoyeva,",
  "ja158:1.4_0": "yo soṇassa sagocaro.",
  "ja158:2.1_0": "Pakkhandinā pagabbhena,",
  "ja158:2.2_0": "niccaṁ sandānakhādinā;",
  "ja158:2.3_0": "Sameti pāpaṁ pāpena,",
  "ja158:2.4_0": "sameti asatā asan”ti.",
  "ja158:3.1_0": "Suhanujātakaṁ aṭṭhamaṁ."
}