{
  "thag2.31:0.1_0": "Theragāthā 2.31",
  "thag2.31:0.2_0": "Dukanipāta",
  "thag2.31:0.3_0": "Catutthavagga",
  "thag2.31:0.4_0": "Migasirattheragāthā",
  "thag2.31:1.1_0": "“Yato ahaṁ pabbajito,",
  "thag2.31:1.2_0": "sammāsambuddhasāsane;",
  "thag2.31:1.3_0": "Vimuccamāno uggacchiṁ,",
  "thag2.31:1.4_0": "kāmadhātuṁ upaccagaṁ.",
  "thag2.31:2.1_0": "Brahmuno pekkhamānassa,",
  "thag2.31:2.2_0": "tato cittaṁ vimucci me;",
  "thag2.31:2.3_0": "Akuppā me vimuttīti,",
  "thag2.31:2.4_0": "sabbasaṁyojanakkhayā”ti.",
  "thag2.31:2.5_0": "… Migasiro thero …."
}