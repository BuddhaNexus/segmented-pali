{
  "tha-ap447:0.1_0": "Therāpadāna",
  "tha-ap447:0.2_0": "Vibhītakavagga",
  "tha-ap447:0.3_0": "5 Uttalipupphiyattheraapadāna",
  "tha-ap447:1.1_0": "“Nigrodhe haritobhāse,",
  "tha-ap447:1.2_0": "saṁviruḷhamhi pādape;",
  "tha-ap447:1.3_0": "Uttalimālaṁ paggayha,",
  "tha-ap447:1.4_0": "bodhiyā abhiropayiṁ.",
  "tha-ap447:2.1_0": "Imasmiṁyeva kappamhi,",
  "tha-ap447:2.2_0": "yaṁ bodhimabhipūjayiṁ;",
  "tha-ap447:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap447:2.4_0": "bodhipūjāyidaṁ phalaṁ.",
  "tha-ap447:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap447:3.2_0": "…pe…",
  "tha-ap447:3.3_0": "viharāmi anāsavo.",
  "tha-ap447:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap447:4.2_0": "…pe…",
  "tha-ap447:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap447:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap447:5.2_0": "…pe…",
  "tha-ap447:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap447:6.1_0": "Itthaṁ sudaṁ āyasmā uttalipupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap447:7.1_0": "Uttalipupphiyattherassāpadānaṁ pañcamaṁ."
}