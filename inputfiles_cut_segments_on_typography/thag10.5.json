{
  "thag10.5:0.1_0": "Theragāthā 10.5",
  "thag10.5:0.2_0": "Dasakanipāta",
  "thag10.5:0.3_0": "Paṭhamavagga",
  "thag10.5:0.4_0": "Kappattheragāthā",
  "thag10.5:1.1_0": "“Nānākulamalasampuṇṇo,",
  "thag10.5:1.2_0": "mahāukkārasambhavo;",
  "thag10.5:1.3_0": "Candanikaṁva paripakkaṁ,",
  "thag10.5:1.4_0": "mahāgaṇḍo mahāvaṇo.",
  "thag10.5:2.1_0": "Pubbaruhirasampuṇṇo,",
  "thag10.5:2.2_0": "gūthakūpena gāḷhito;",
  "thag10.5:2.3_0": "Āpopaggharaṇo kāyo,",
  "thag10.5:2.4_0": "sadā sandati pūtikaṁ.",
  "thag10.5:3.1_0": "Saṭṭhikaṇḍarasambandho,",
  "thag10.5:3.2_0": "maṁsalepanalepito;",
  "thag10.5:3.3_0": "Cammakañcukasannaddho,",
  "thag10.5:3.4_0": "pūtikāyo niratthako.",
  "thag10.5:4.1_0": "Aṭṭhisaṅghātaghaṭito,",
  "thag10.5:4.2_0": "nhārusuttanibandhano;",
  "thag10.5:4.3_0": "Nekesaṁ saṅgatībhāvā,",
  "thag10.5:4.4_0": "kappeti iriyāpathaṁ.",
  "thag10.5:5.1_0": "Dhuvappayāto maraṇāya,",
  "thag10.5:5.2_0": "maccurājassa santike;",
  "thag10.5:5.3_0": "Idheva chaḍḍayitvāna,",
  "thag10.5:5.4_0": "yenakāmaṅgamo naro.",
  "thag10.5:6.1_0": "Avijjāya nivuto kāyo,",
  "thag10.5:6.2_0": "catuganthena ganthito;",
  "thag10.5:6.3_0": "Oghasaṁsīdano kāyo,",
  "thag10.5:6.4_0": "anusayajālamotthato.",
  "thag10.5:7.1_0": "Pañcanīvaraṇe yutto,",
  "thag10.5:7.2_0": "vitakkena samappito;",
  "thag10.5:7.3_0": "Taṇhāmūlenānugato,",
  "thag10.5:7.4_0": "mohacchādanachādito.",
  "thag10.5:8.1_0": "Evāyaṁ vattate kāyo,",
  "thag10.5:8.2_0": "kammayantena yantito;",
  "thag10.5:8.3_0": "Sampatti ca vipatyantā,",
  "thag10.5:8.4_0": "nānābhāvo vipajjati.",
  "thag10.5:9.1_0": "Yemaṁ kāyaṁ mamāyanti,",
  "thag10.5:9.2_0": "andhabālā puthujjanā;",
  "thag10.5:9.3_0": "Vaḍḍhenti kaṭasiṁ ghoraṁ,",
  "thag10.5:9.4_0": "ādiyanti punabbhavaṁ.",
  "thag10.5:10.1_0": "Yemaṁ kāyaṁ vivajjenti,",
  "thag10.5:10.2_0": "Gūthalittaṁva pannagaṁ;",
  "thag10.5:10.3_0": "Bhavamūlaṁ vamitvāna,",
  "thag10.5:10.4_0": "Parinibbissantināsavā”ti.",
  "thag10.5:10.5_0": "… Kappo thero …."
}