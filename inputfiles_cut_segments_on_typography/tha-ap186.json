{
  "tha-ap186:0.1_0": "Therāpadāna",
  "tha-ap186:0.2_0": "Kuṭajapupphiyavagga",
  "tha-ap186:0.3_0": "4. Pañcahatthiyattheraapadāna",
  "tha-ap186:1.1_0": "“Tisso nāmāsi bhagavā,",
  "tha-ap186:1.2_0": "lokajeṭṭho narāsabho;",
  "tha-ap186:1.3_0": "Purakkhato sāvakehi,",
  "tha-ap186:1.4_0": "rathiyaṁ paṭipajjatha.",
  "tha-ap186:2.1_0": "Pañca uppalahatthā ca,",
  "tha-ap186:2.2_0": "cāturā ṭhapitā mayā;",
  "tha-ap186:2.3_0": "Āhutiṁ dātukāmohaṁ,",
  "tha-ap186:2.4_0": "paggaṇhiṁ vatasiddhiyā.",
  "tha-ap186:3.1_0": "Suvaṇṇavaṇṇaṁ sambuddhaṁ,",
  "tha-ap186:3.2_0": "Gacchantaṁ antarāpaṇe;",
  "tha-ap186:3.3_0": "Buddharaṁsīhi phuṭṭhosmi,",
  "tha-ap186:3.4_0": "Pūjesiṁ dvipaduttamaṁ.",
  "tha-ap186:4.1_0": "Dvenavute ito kappe,",
  "tha-ap186:4.2_0": "yaṁ pupphamabhipūjayiṁ;",
  "tha-ap186:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap186:4.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap186:5.1_0": "Ito terasakappamhi,",
  "tha-ap186:5.2_0": "pañca susabhasammatā;",
  "tha-ap186:5.3_0": "Sattaratanasampannā,",
  "tha-ap186:5.4_0": "cakkavattī mahabbalā.",
  "tha-ap186:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap186:6.2_0": "…pe…",
  "tha-ap186:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap186:7.1_0": "Itthaṁ sudaṁ āyasmā pañcahatthiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap186:8.1_0": "Pañcahatthiyattherassāpadānaṁ catutthaṁ."
}