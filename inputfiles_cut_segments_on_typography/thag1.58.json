{
  "thag1.58:0.1_0": "Theragāthā 1.58",
  "thag1.58:0.2_0": "Ekakanipāta",
  "thag1.58:0.3_0": "Chaṭṭhavagga",
  "thag1.58:0.4_0": "Ramaṇīyakuṭikattheragāthā",
  "thag1.58:1.1_0": "“Ramaṇīyā me kuṭikā,",
  "thag1.58:1.2_0": "Saddhādeyyā manoramā;",
  "thag1.58:1.3_0": "Na me attho kumārīhi,",
  "thag1.58:1.4_0": "Yesaṁ attho tahiṁ gacchatha nāriyo”ti.",
  "thag1.58:1.5_0": "… Ramaṇīyakuṭiko thero …."
}