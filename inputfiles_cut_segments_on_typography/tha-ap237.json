{
  "tha-ap237:0.1_0": "Therāpadāna",
  "tha-ap237:0.2_0": "Udakāsanavagga",
  "tha-ap237:0.3_0": "5 Vedikārakattheraapadāna",
  "tha-ap237:1.1_0": "“Vipassino bhagavato,",
  "tha-ap237:1.2_0": "bodhiyā pādaputtame;",
  "tha-ap237:1.3_0": "Pasannacitto sumano,",
  "tha-ap237:1.4_0": "kāresiṁ vedikaṁ ahaṁ.",
  "tha-ap237:2.1_0": "Ekanavutito kappe,",
  "tha-ap237:2.2_0": "kāresiṁ vedikaṁ ahaṁ;",
  "tha-ap237:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap237:2.4_0": "vedikāya idaṁ phalaṁ.",
  "tha-ap237:3.1_0": "Ito ekādase kappe,",
  "tha-ap237:3.2_0": "ahosiṁ sūriyassamo;",
  "tha-ap237:3.3_0": "Sattaratanasampanno,",
  "tha-ap237:3.4_0": "cakkavattī mahabbalo.",
  "tha-ap237:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap237:4.2_0": "…pe…",
  "tha-ap237:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap237:5.1_0": "Itthaṁ sudaṁ āyasmā vedikārako thero imā gāthāyo abhāsitthāti.",
  "tha-ap237:6.1_0": "Vedikārakattherassāpadānaṁ pañcamaṁ."
}