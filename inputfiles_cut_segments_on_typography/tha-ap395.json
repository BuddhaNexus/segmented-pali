{
  "tha-ap395:0.1_0": "Therāpadāna",
  "tha-ap395:0.2_0": "Pilindavacchavagga",
  "tha-ap395:0.3_0": "3. Sabbakittikattheraapadāna",
  "tha-ap395:1.1_0": "“Kaṇikāraṁva jalitaṁ,",
  "tha-ap395:1.2_0": "dīparukkhaṁva ujjalaṁ;",
  "tha-ap395:1.3_0": "Osadhiṁva virocantaṁ,",
  "tha-ap395:1.4_0": "vijjutaṁ gagane yathā.",
  "tha-ap395:2.1_0": "Asambhītaṁ anuttāsiṁ,",
  "tha-ap395:2.2_0": "migarājaṁva kesariṁ;",
  "tha-ap395:2.3_0": "Ñāṇālokaṁ pakāsentaṁ,",
  "tha-ap395:2.4_0": "maddantaṁ titthiye gaṇe.",
  "tha-ap395:3.1_0": "Uddharantaṁ imaṁ lokaṁ,",
  "tha-ap395:3.2_0": "chindantaṁ sabbasaṁsayaṁ;",
  "tha-ap395:3.3_0": "Gajjantaṁ migarājaṁva,",
  "tha-ap395:3.4_0": "addasaṁ lokanāyakaṁ.",
  "tha-ap395:4.1_0": "Jaṭājinadharo āsiṁ,",
  "tha-ap395:4.2_0": "brahā uju patāpavā;",
  "tha-ap395:4.3_0": "Vākacīraṁ gahetvāna,",
  "tha-ap395:4.4_0": "pādamūle apatthariṁ.",
  "tha-ap395:5.1_0": "Kāḷānusāriyaṁ gayha,",
  "tha-ap395:5.2_0": "anulimpiṁ tathāgataṁ;",
  "tha-ap395:5.3_0": "Sambuddhamanulimpetvā,",
  "tha-ap395:5.4_0": "santhaviṁ lokanāyakaṁ.",
  "tha-ap395:6.1_0": "‘Samuddharasimaṁ lokaṁ,",
  "tha-ap395:6.2_0": "oghatiṇṇamahāmuni;",
  "tha-ap395:6.3_0": "Ñāṇālokena jotesi,",
  "tha-ap395:6.4_0": "nāvaṭaṁ ñāṇamuttamaṁ.",
  "tha-ap395:7.1_0": "Dhammacakkaṁ pavattesi,",
  "tha-ap395:7.2_0": "maddase paratitthiye;",
  "tha-ap395:7.3_0": "Usabho jitasaṅgāmo,",
  "tha-ap395:7.4_0": "sampakampesi medaniṁ.",
  "tha-ap395:8.1_0": "Mahāsamudde ūmiyo,",
  "tha-ap395:8.2_0": "velantamhi pabhijjare;",
  "tha-ap395:8.3_0": "Tatheva tava ñāṇamhi,",
  "tha-ap395:8.4_0": "sabbadiṭṭhī pabhijjare.",
  "tha-ap395:9.1_0": "Sukhumacchikajālena,",
  "tha-ap395:9.2_0": "saramhi sampatānite;",
  "tha-ap395:9.3_0": "Antojālikatā pāṇā,",
  "tha-ap395:9.4_0": "pīḷitā honti tāvade.",
  "tha-ap395:10.1_0": "Tatheva titthiyā loke,",
  "tha-ap395:10.2_0": "puthupāsaṇḍanissitā;",
  "tha-ap395:10.3_0": "Antoñāṇavare tuyhaṁ,",
  "tha-ap395:10.4_0": "parivattanti mārisa.",
  "tha-ap395:11.1_0": "Patiṭṭhā vuyhataṁ oghe,",
  "tha-ap395:11.2_0": "tvañhi nātho abandhunaṁ;",
  "tha-ap395:11.3_0": "Bhayaṭṭitānaṁ saraṇaṁ,",
  "tha-ap395:11.4_0": "muttitthīnaṁ parāyaṇaṁ.",
  "tha-ap395:12.1_0": "Ekavīro asadiso,",
  "tha-ap395:12.2_0": "mettākaruṇasañcayo;",
  "tha-ap395:12.3_0": "Asamo susamo santo,",
  "tha-ap395:12.4_0": "vasī tādī jitañjayo.",
  "tha-ap395:13.1_0": "Dhīro vigatasammoho,",
  "tha-ap395:13.2_0": "anejo akathaṅkathī;",
  "tha-ap395:13.3_0": "Tusito vantadososi,",
  "tha-ap395:13.4_0": "nimmalo saṁyato suci.",
  "tha-ap395:14.1_0": "Saṅgātigo hatamado,",
  "tha-ap395:14.2_0": "tevijjo tibhavantago;",
  "tha-ap395:14.3_0": "Sīmātigo dhammagaru,",
  "tha-ap395:14.4_0": "gatattho hitavabbhuto.",
  "tha-ap395:15.1_0": "Tārako tvaṁ yathā nāvā,",
  "tha-ap395:15.2_0": "nidhīvassāsakārako;",
  "tha-ap395:15.3_0": "Asambhīto yathā sīho,",
  "tha-ap395:15.4_0": "gajarājāva dappito’.",
  "tha-ap395:16.1_0": "Thometvā dasagāthāhi,",
  "tha-ap395:16.2_0": "padumuttaraṁ mahāyasaṁ;",
  "tha-ap395:16.3_0": "Vanditvā satthuno pāde,",
  "tha-ap395:16.4_0": "tuṇhī aṭṭhāsahaṁ tadā.",
  "tha-ap395:17.1_0": "Padumuttaro lokavidū,",
  "tha-ap395:17.2_0": "āhutīnaṁ paṭiggaho;",
  "tha-ap395:17.3_0": "Bhikkhusaṅghe ṭhito satthā,",
  "tha-ap395:17.4_0": "imā gāthā abhāsatha.",
  "tha-ap395:18.1_0": "‘Yo me sīlañca ñāṇañca,",
  "tha-ap395:18.2_0": "saddhammañcāpi vaṇṇayi;",
  "tha-ap395:18.3_0": "Tamahaṁ kittayissāmi,",
  "tha-ap395:18.4_0": "suṇātha mama bhāsato.",
  "tha-ap395:19.1_0": "Saṭṭhi kappasahassāni,",
  "tha-ap395:19.2_0": "devaloke ramissati;",
  "tha-ap395:19.3_0": "Aññe devebhibhavitvā,",
  "tha-ap395:19.4_0": "issaraṁ kārayissati.",
  "tha-ap395:20.1_0": "So pacchā pabbajitvāna,",
  "tha-ap395:20.2_0": "sukkamūlena codito;",
  "tha-ap395:20.3_0": "Gotamassa bhagavato,",
  "tha-ap395:20.4_0": "sāsane pabbajissati.",
  "tha-ap395:21.1_0": "Pabbajitvāna kāyena,",
  "tha-ap395:21.2_0": "pāpakammaṁ vivajjiya;",
  "tha-ap395:21.3_0": "Sabbāsave pariññāya,",
  "tha-ap395:21.4_0": "nibbāyissatināsavo’.",
  "tha-ap395:22.1_0": "Yathāpi megho thanayaṁ,",
  "tha-ap395:22.2_0": "tappeti mediniṁ imaṁ;",
  "tha-ap395:22.3_0": "Tatheva tvaṁ mahāvīra,",
  "tha-ap395:22.4_0": "dhammena tappayī mamaṁ.",
  "tha-ap395:23.1_0": "Sīlaṁ paññañca dhammañca,",
  "tha-ap395:23.2_0": "thavitvā lokanāyakaṁ;",
  "tha-ap395:23.3_0": "Pattomhi paramaṁ santiṁ,",
  "tha-ap395:23.4_0": "nibbānaṁ padamaccutaṁ.",
  "tha-ap395:24.1_0": "Aho nūna sa bhagavā,",
  "tha-ap395:24.2_0": "Ciraṁ tiṭṭheyya cakkhumā;",
  "tha-ap395:24.3_0": "Aññātañca vijāneyyuṁ,",
  "tha-ap395:24.4_0": "Phuseyyuṁ amataṁ padaṁ.",
  "tha-ap395:25.1_0": "Ayaṁ me pacchimā jāti,",
  "tha-ap395:25.2_0": "bhavā sabbe samūhatā;",
  "tha-ap395:25.3_0": "Sabbāsave pariññāya,",
  "tha-ap395:25.4_0": "viharāmi anāsavo.",
  "tha-ap395:26.1_0": "Satasahassito kappe,",
  "tha-ap395:26.2_0": "yaṁ buddhamabhithomayiṁ;",
  "tha-ap395:26.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap395:26.4_0": "kittanāya idaṁ phalaṁ.",
  "tha-ap395:27.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap395:27.2_0": "bhavā sabbe samūhatā;",
  "tha-ap395:27.3_0": "Sabbāsavā parikkhīṇā,",
  "tha-ap395:27.4_0": "natthi dāni punabbhavo.",
  "tha-ap395:28.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap395:28.2_0": "…pe…",
  "tha-ap395:28.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap395:29.1_0": "Paṭisambhidā catasso,",
  "tha-ap395:29.2_0": "…pe…",
  "tha-ap395:29.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap395:30.1_0": "Itthaṁ sudaṁ āyasmā sabbakittiko thero imā gāthāyo abhāsitthāti.",
  "tha-ap395:31.1_0": "Sabbakittikattherassāpadānaṁ tatiyaṁ."
}