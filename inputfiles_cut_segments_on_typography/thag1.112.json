{
  "thag1.112:0.1_0": "Theragāthā 1.112",
  "thag1.112:0.2_0": "Ekakanipāta",
  "thag1.112:0.3_0": "Dvādasamavagga",
  "thag1.112:0.4_0": "Vacchagottattheragāthā",
  "thag1.112:1.1_0": "“Tevijjohaṁ mahājhāyī,",
  "thag1.112:1.2_0": "cetosamathakovido;",
  "thag1.112:1.3_0": "Sadattho me anuppatto,",
  "thag1.112:1.4_0": "kataṁ buddhassa sāsanan”ti.",
  "thag1.112:1.5_0": "… Vacchagotto thero …."
}