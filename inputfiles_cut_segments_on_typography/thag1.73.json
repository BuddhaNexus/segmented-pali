{
  "thag1.73:0.1_0": "Theragāthā 1.73",
  "thag1.73:0.2_0": "Ekakanipāta",
  "thag1.73:0.3_0": "Aṭṭhamavagga",
  "thag1.73:0.4_0": "Māṇavattheragāthā",
  "thag1.73:1.1_0": "“Jiṇṇañca disvā dukhitañca byādhitaṁ,",
  "thag1.73:1.2_0": "Matañca disvā gatamāyusaṅkhayaṁ;",
  "thag1.73:1.3_0": "Tato ahaṁ nikkhamitūna pabbajiṁ,",
  "thag1.73:1.4_0": "Pahāya kāmāni manoramānī”ti.",
  "thag1.73:1.5_0": "… Māṇavo thero …."
}