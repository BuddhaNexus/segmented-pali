{
  "atk-s0304a8:0_0": "9. Asaṅkhatasaṁyuttaṁ",
  "atk-s0304a8:1_0": "1. Paṭhamavaggo",
  "atk-s0304a8:2_0": "1-11. Kāyagatāsatisuttādivaṇṇanā",
  "atk-s0304a8:3_0": "366- 376.",
  "atk-s0304a8:3_1": "Asaṅkhatasaṁyutte asaṅkhatanti akataṁ.",
  "atk-s0304a8:3_2": "Hitesināti hitaṁ esantena.",
  "atk-s0304a8:3_3": "Anukampakenāti anukampamānena.",
  "atk-s0304a8:3_4": "Anukampaṁ upādāyāti anukampaṁ cittena pariggahetvā,",
  "atk-s0304a8:3_5": "paṭiccātipi vuttaṁ hoti.",
  "atk-s0304a8:3_6": "Kataṁ vo taṁ mayāti taṁ mayā imaṁ asaṅkhatañca asaṅkhatamaggañca desentena tumhākaṁ kataṁ.",
  "atk-s0304a8:3_7": "Ettakameva hi anukampakassa satthu kiccaṁ,",
  "atk-s0304a8:3_8": "yadidaṁ aviparītadhammadesanā.",
  "atk-s0304a8:3_9": "Ito paraṁ pana paṭipatti nāma sāvakānaṁ kiccaṁ.",
  "atk-s0304a8:3_10": "Tenāha etāni,",
  "atk-s0304a8:3_11": "bhikkhave,",
  "atk-s0304a8:3_12": "rukkhamūlāni…pe…",
  "atk-s0304a8:3_13": "amhākaṁ anusāsanīti iminā rukkhamūlasenāsanaṁ dasseti.",
  "atk-s0304a8:3_14": "Suññāgārānīti iminā janavivittaṁ ṭhānaṁ.",
  "atk-s0304a8:3_15": "Ubhayena ca yogānurūpaṁ senāsanaṁ ācikkhati,",
  "atk-s0304a8:3_16": "dāyajjaṁ niyyāteti.",
  "atk-s0304a8:4_0": "Jhāyathāti ārammaṇūpanijjhānena aṭṭhatiṁsārammaṇāni,",
  "atk-s0304a8:4_1": "lakkhaṇūpanijjhānena ca aniccādito khandhāyatanādīni upanijjhāyatha,",
  "atk-s0304a8:4_2": "samathañca vipassanañca vaḍḍhethāti vuttaṁ hoti.",
  "atk-s0304a8:4_3": "Mā pamādatthāti mā pamajjittha.",
  "atk-s0304a8:4_4": "Mā pacchā vippaṭisārino ahuvatthāti ye hi pubbe daharakāle arogakāle sattasappāyādisampattikāle satthu",
  "atk-s0304a8:4_5": "sammukhībhāvakāle ca yonisomanasikārarahitā rattindivaṁ maṅkulabhattaṁ hutvā seyyasukhaṁ middhasukhaṁ",
  "atk-s0304a8:4_6": "anubhontā pamajjanti,",
  "atk-s0304a8:4_7": "te pacchā jarākāle rogakāle maraṇakāle vipattikāle satthu parinibbutakāle ca taṁ pubbe pamādavihāraṁ",
  "atk-s0304a8:4_8": "anussarantā sappaṭisandhikālakiriyañca bhāriyaṁ sampassamānā vippaṭisārino honti.",
  "atk-s0304a8:4_9": "Tumhe pana tādisā mā ahuvatthāti dassento āha “mā pacchā vippaṭisārino ahuvatthā”ti.",
  "atk-s0304a8:5_0": "Ayaṁ vo amhākaṁ anusāsanīti ayaṁ amhākaṁ santikā “jhāyatha mā pamādatthā”ti tumhākaṁ anusāsanī,",
  "atk-s0304a8:5_1": "ovādoti vuttaṁ hoti.",
  "atk-s0304a8:6_0": "2. Dutiyavaggo",
  "atk-s0304a8:7_0": "1-33. Asaṅkhatasuttādivaṇṇanā",
  "atk-s0304a8:8_0": "377-409.Kāyekāyānupassītiādīsu yaṁ vattabbaṁ, taṁ parato vakkhāma.",
  "atk-s0304a8:9_0": "Anatantiādīsu taṇhānatiyā abhāvena anataṁ.",
  "atk-s0304a8:9_1": "Catunnaṁ āsavānaṁ abhāvena anāsavaṁ.",
  "atk-s0304a8:9_2": "Paramatthasaccatāya saccaṁ.",
  "atk-s0304a8:9_3": "Vaṭṭassa parabhāgaṭṭhena pāraṁ.",
  "atk-s0304a8:9_4": "Saṇhaṭṭhena nipuṇaṁ.",
  "atk-s0304a8:9_5": "Suṭṭhu duddasatāya sududdasaṁ.",
  "atk-s0304a8:9_6": "Jarāya ajaritattā ajajjaraṁ.",
  "atk-s0304a8:9_7": "Thiraṭṭhena dhuvaṁ.",
  "atk-s0304a8:9_8": "Apalujjanatāya apalokitaṁ.",
  "atk-s0304a8:9_9": "Cakkhuviññāṇena apassitabbattā anidassanaṁ.",
  "atk-s0304a8:9_10": "Taṇhāmānadiṭṭhipapañcānaṁ abhāvena nippapañcaṁ.",
  "atk-s0304a8:10_0": "Santabhāvaṭṭhena santaṁ.",
  "atk-s0304a8:10_1": "Maraṇābhāvena amataṁ.",
  "atk-s0304a8:10_2": "Uttamaṭṭhena paṇītaṁ.",
  "atk-s0304a8:10_3": "Sassirikaṭṭhena sivaṁ.",
  "atk-s0304a8:10_4": "Nirupaddavatāya khemaṁ.",
  "atk-s0304a8:10_5": "Taṇhākkhayassa paccayattā taṇhakkhayaṁ.",
  "atk-s0304a8:11_0": "Vimhāpanīyaṭṭhena accharaṁ paharitabbayuttakanti acchariyaṁ.",
  "atk-s0304a8:11_1": "Abhūtameva bhūtaṁ ajātaṁ hutvā atthīti vā abbhutaṁ.",
  "atk-s0304a8:11_2": "Niddukkhattā anītikaṁ.",
  "atk-s0304a8:11_3": "Niddukkhasabhāvattā anītikadhammaṁ.",
  "atk-s0304a8:11_4": "Vānābhāvena nibbānaṁ.",
  "atk-s0304a8:11_5": "Byābajjhābhāveneva abyābajjhaṁ.",
  "atk-s0304a8:11_6": "Virāgādhigamassa paccayato virāgaṁ.",
  "atk-s0304a8:11_7": "Paramatthasuddhitāya suddhi.",
  "atk-s0304a8:11_8": "Tīhi bhavehi muttatāya mutti.",
  "atk-s0304a8:11_9": "Kāmālayānaṁ abhāvena anālayaṁ.",
  "atk-s0304a8:11_10": "Patiṭṭhaṭṭhena dīpaṁ.",
  "atk-s0304a8:11_11": "Allīyitabbayuttaṭṭhena leṇaṁ.",
  "atk-s0304a8:11_12": "Tāyanaṭṭhena tāṇaṁ.",
  "atk-s0304a8:11_13": "Bhayasaraṇaṭṭhena saraṇaṁ,",
  "atk-s0304a8:11_14": "bhayanāsananti attho.",
  "atk-s0304a8:11_15": "Paraṁ ayanaṁ gati patiṭṭhāti parāyaṇaṁ.",
  "atk-s0304a8:11_16": "Sesamettha vuttanayamevāti.",
  "atk-s0304a8:12_0": "Asaṅkhatasaṁyuttavaṇṇanā niṭṭhitā."
}