{
  "anya-e1009n5:0_0": "Abhiyācaka",
  "anya-e1009n5:1_0": "4.",
  "anya-e1009n5:2_0": "Dhamma-saṅghaṁ vanditvāna,",
  "anya-e1009n5:3_0": "Sabba-lokassa nāyakaṁ;",
  "anya-e1009n5:4_0": "Yācito tikakrapa-therena,",
  "anya-e1009n5:5_0": "Māṇavena ca dhīmatā.",
  "anya-e1009n5:6_0": "5.",
  "anya-e1009n5:7_0": "Uttānameva saṅkhepaṁ,",
  "anya-e1009n5:8_0": "Nānā-sattha-sudhāritaṁ;",
  "anya-e1009n5:9_0": "Nara-dakkhaṁ likhissami,",
  "anya-e1009n5:10_0": "Passantu dhīra-māmakā.",
  "anya-e1009n5:11_0": "6.",
  "anya-e1009n5:12_0": "Kosajjaṁ bhayato disvā,",
  "anya-e1009n5:13_0": "Vīriyañjāpi khemato;",
  "anya-e1009n5:14_0": "Āraddhavīriyā hotha;",
  "anya-e1009n5:15_0": "Esā buddhānusāsanī.",
  "anya-e1009n5:16_0": "7.",
  "anya-e1009n5:17_0": "Vīriyavā kho bhikkhave ariyasāvako,",
  "anya-e1009n5:18_0": "Akusalaṁ pajahati, kusalaṁ bhāveti;",
  "anya-e1009n5:19_0": "Sāvajjaṁ pajahati, anavajjaṁ bhāveti,",
  "anya-e1009n5:20_0": "Suddhamattānaṁ pariharatīti.",
  "anya-e1009n5:21_0": "Vīriyavato kiṁ nāma kammaṁ na sijjhati;",
  "anya-e1009n5:22_0": "Purisakāro nāma na nassati,",
  "anya-e1009n5:23_0": "Sukhe patiṭṭhāpetīti jānāmi.",
  "anya-e1009n5:24_0": "8.",
  "anya-e1009n5:25_0": "Yathā khittaṁ nabhe leḍḍu,",
  "anya-e1009n5:26_0": "Dhuvaṁ patati bhūmiyaṁ;",
  "anya-e1009n5:27_0": "Tatheva buddha-seṭṭhānaṁ,",
  "anya-e1009n5:28_0": "Vacanaṁ dhuva-sassataṁ.",
  "anya-e1009n5:29_0": "9.",
  "anya-e1009n5:30_0": "A-dvejjhavacanā buddhā,",
  "anya-e1009n5:31_0": "A-moghavacanā jinā.",
  "anya-e1009n5:32_0": "10.",
  "anya-e1009n5:33_0": "Sussusā labhate paññaṁ,",
  "anya-e1009n5:34_0": "Uṭṭhātā vindate dhanaṁ;",
  "anya-e1009n5:35_0": "Tasmā pāḷiṁ guruṁ katvā,",
  "anya-e1009n5:36_0": "Imaṁ passāhi sobhaṇaṁ.",
  "anya-e1009n5:37_0": "11.",
  "anya-e1009n5:38_0": "Sussusā suta-buddhinī,",
  "anya-e1009n5:39_0": "Sutaṁ paññāya vaḍḍhanaṁ;",
  "anya-e1009n5:40_0": "Paññāya atthaṁ jānāti,",
  "anya-e1009n5:41_0": "Ñāto attho sukhāvaho.",
  "anya-e1009n5:42_0": "12.",
  "anya-e1009n5:43_0": "Satataṁjjhāyanaṁ vāda,",
  "anya-e1009n5:44_0": "Para-tantavalokanaṁ;",
  "anya-e1009n5:45_0": "Sabbijjācera-sevāca,",
  "anya-e1009n5:46_0": "Buddhi-mati-karo guṇo.",
  "anya-e1009n5:47_0": "13.",
  "anya-e1009n5:48_0": "Ati-dīghova nīgho hi,",
  "anya-e1009n5:49_0": "Kusīto hīna-vīriyo;",
  "anya-e1009n5:50_0": "Tasmā vīriyaṁ katvāna,",
  "anya-e1009n5:51_0": "Vijjaṁ esantu sādhavo.",
  "anya-e1009n5:52_0": "14.",
  "anya-e1009n5:53_0": "Suporiso tāva sippaṁ,",
  "anya-e1009n5:54_0": "Uggaṇheyya paraṁ dhanaṁ;",
  "anya-e1009n5:55_0": "Gaveseyya tato mantaṁ,",
  "anya-e1009n5:56_0": "Katheyya sacca-bhāsitaṁ.",
  "anya-e1009n5:57_0": "15.",
  "anya-e1009n5:58_0": "Pathamaṁ na parājaye sippaṁ,",
  "anya-e1009n5:59_0": "Dutīyaṁ na parājaye dhanaṁ;",
  "anya-e1009n5:60_0": "Tatīyaṁ na parājaye dhanaṁ,",
  "anya-e1009n5:61_0": "Catutthamatthaṁ kiṁ karissati.",
  "anya-e1009n5:62_0": "16.",
  "anya-e1009n5:63_0": "Sobhanti a-milātāni,",
  "anya-e1009n5:64_0": "Pupphāniva pilandhituṁ;",
  "anya-e1009n5:65_0": "Tathā sobhanti dārakā,",
  "anya-e1009n5:66_0": "Yobbaneyeva sikkhituṁ.",
  "anya-e1009n5:67_0": "17.",
  "anya-e1009n5:68_0": "Tasmā have guṇādhāraṁ,",
  "anya-e1009n5:69_0": "Paññā-vaḍḍhanamuttamaṁ;",
  "anya-e1009n5:70_0": "Sikkheyya matimā poso,",
  "anya-e1009n5:71_0": "Patthento hitamattano.",
  "anya-e1009n5:72_0": "18.",
  "anya-e1009n5:73_0": "Alaṁ vāyamituṁ sippe,",
  "anya-e1009n5:74_0": "Attha-kāmena jantunā;",
  "anya-e1009n5:75_0": "Kataṁ vijaññā vijjādi,",
  "anya-e1009n5:76_0": "Vayo te mā upajjhagā.",
  "anya-e1009n5:77_0": "19.",
  "anya-e1009n5:78_0": "Vijjaṁ sikkhe, care sīlaṁ,",
  "anya-e1009n5:79_0": "Dhīrena saha saṁvase;",
  "anya-e1009n5:80_0": "Dhanācaye, kare kammaṁ,",
  "anya-e1009n5:81_0": "Piyaṁ vācañca saṁvade.",
  "anya-e1009n5:82_0": "20.",
  "anya-e1009n5:83_0": "Na tveva supituṁ hoti,",
  "anya-e1009n5:84_0": "Ratti nakkhatta-mālinī;",
  "anya-e1009n5:85_0": "Paṭijaggitumevesā,",
  "anya-e1009n5:86_0": "Ratti hoti vijānataṁ.",
  "anya-e1009n5:87_0": "21.",
  "anya-e1009n5:88_0": "Uṭṭhāhatha nisīdatha,",
  "anya-e1009n5:89_0": "Ko attho supitena vo;",
  "anya-e1009n5:90_0": "Sādhu kho sippa-vijjāhvā,",
  "anya-e1009n5:91_0": "Vijjaṁ sikkhatha santataṁ.",
  "anya-e1009n5:92_0": "22.",
  "anya-e1009n5:93_0": "Ārabbhatha sadā puttā,",
  "anya-e1009n5:94_0": "Bahussutaṁ gavesituṁ;",
  "anya-e1009n5:95_0": "Yasmā loke sippavantā,",
  "anya-e1009n5:96_0": "Sabbā-disāsu pākaṭā.",
  "anya-e1009n5:97_0": "23.",
  "anya-e1009n5:98_0": "Sakyarūpaṁ pure santaṁ,",
  "anya-e1009n5:99_0": "Mayā sippaṁ na sikkhitaṁ;",
  "anya-e1009n5:100_0": "Kicchā vutti a-sippassa,",
  "anya-e1009n5:101_0": "Iti pacchā nutappati.",
  "anya-e1009n5:102_0": "24.",
  "anya-e1009n5:103_0": "Lokatthaṁ loka-kammantaṁ,",
  "anya-e1009n5:104_0": "Icchanto pariyesituṁ;",
  "anya-e1009n5:105_0": "Niccameva vīriyañca,",
  "anya-e1009n5:106_0": "Atthaṁ mantañja cintaye.",
  "anya-e1009n5:107_0": "25.",
  "anya-e1009n5:108_0": "Dhanavā guṇavā loke,",
  "anya-e1009n5:109_0": "Sabbā-disāya pākaṭo;",
  "anya-e1009n5:110_0": "Sīlavā paññavā macco,",
  "anya-e1009n5:111_0": "Sabba-lokehi pūjito.",
  "anya-e1009n5:112_0": "26.",
  "anya-e1009n5:113_0": "Sajīvati yaso yassa,",
  "anya-e1009n5:114_0": "Kitti yassa sajīvati;",
  "anya-e1009n5:115_0": "Yasa-kitti vihīnassa,",
  "anya-e1009n5:116_0": "Jīvantopi matopamā.",
  "anya-e1009n5:117_0": "27.",
  "anya-e1009n5:118_0": "Saddhīdha vittaṁ purisassa seṭṭhaṁ,",
  "anya-e1009n5:119_0": "Dhammo suciṇṇo sukhamāvahāti;",
  "anya-e1009n5:120_0": "Saccaṁ have sādutaraṁ rasānaṁ,",
  "anya-e1009n5:121_0": "Paññājīviṁ jīvitamāhu seṭṭhanti.",
  "anya-e1009n5:122_0": "28.",
  "anya-e1009n5:123_0": "Satimato sadā bhaddaṁ,",
  "anya-e1009n5:124_0": "Satimā sukhamedhati;",
  "anya-e1009n5:125_0": "Satimato suve seyyo,",
  "anya-e1009n5:126_0": "Verā ca parimuccati.",
  "anya-e1009n5:127_0": "29.",
  "anya-e1009n5:128_0": "Mā vo khaṇaṁ virādhetha,",
  "anya-e1009n5:129_0": "Khaṇātītā hi socare;",
  "anya-e1009n5:130_0": "Sadatthe vāyameyyātha,",
  "anya-e1009n5:131_0": "Khaṇo vo paṭipādito.",
  "anya-e1009n5:132_0": "30.",
  "anya-e1009n5:133_0": "Yathicchitaṁ na pappoti,",
  "anya-e1009n5:134_0": "A-phiyo nāvikoṇṇave;",
  "anya-e1009n5:135_0": "Tathevāvīriyopettha,",
  "anya-e1009n5:136_0": "Tasmārabheyya sāsanaṁ.",
  "anya-e1009n5:137_0": "31.",
  "anya-e1009n5:138_0": "Vāyametheva puriso,",
  "anya-e1009n5:139_0": "Yāva atthassa nipphadā;",
  "anya-e1009n5:140_0": "Nipphannasobhaṇo attho,",
  "anya-e1009n5:141_0": "Khantā bhiyyo na vijjati.",
  "anya-e1009n5:142_0": "32.",
  "anya-e1009n5:143_0": "Sameva ñāṇa-vāyāme,",
  "anya-e1009n5:144_0": "Sukhāvaho su-maṅgalo;",
  "anya-e1009n5:145_0": "Ñunedhike tathā no hi,",
  "anya-e1009n5:146_0": "Dvayena sādhu sampadā.",
  "anya-e1009n5:147_0": "33.",
  "anya-e1009n5:148_0": "Kāya-kammāni sijjhanti,",
  "anya-e1009n5:149_0": "Vacī-kammāni vīriyaṁ;",
  "anya-e1009n5:150_0": "Na hi kiccāni cintāhi,",
  "anya-e1009n5:151_0": "Kareyyāthīdha vāyamaṁ.",
  "anya-e1009n5:152_0": "34.",
  "anya-e1009n5:153_0": "Paṭikacceva kareyya,",
  "anya-e1009n5:154_0": "Taṁ jaññā hitamattano;",
  "anya-e1009n5:155_0": "Na sākaṭikacintāya,",
  "anya-e1009n5:156_0": "Mandā dhīro parakkame.",
  "anya-e1009n5:157_0": "35.",
  "anya-e1009n5:158_0": "Thirena saka-kammena,",
  "anya-e1009n5:159_0": "Vaḍḍhatiyeva saṁ phalaṁ;",
  "anya-e1009n5:160_0": "A-thirena alasena,",
  "anya-e1009n5:161_0": "Kara-kammaṁ phalañca no.",
  "anya-e1009n5:162_0": "36.",
  "anya-e1009n5:163_0": "Āsīsetheva puriso,",
  "anya-e1009n5:164_0": "Na nibbindeyya paṇḍito;",
  "anya-e1009n5:165_0": "Passāmi vohaṁ attānaṁ,",
  "anya-e1009n5:166_0": "Yathā icchiṁ tathā ahu.",
  "anya-e1009n5:167_0": "37.",
  "anya-e1009n5:168_0": "Aṇaṇo ñātīnaṁ hoti,",
  "anya-e1009n5:169_0": "Devānaṁ pitunañca so;",
  "anya-e1009n5:170_0": "Karaṁ purisa-kiccāni,",
  "anya-e1009n5:171_0": "Na ca pacchānutappati.",
  "anya-e1009n5:172_0": "38.",
  "anya-e1009n5:173_0": "Hiyyoti hiyyati poso,",
  "anya-e1009n5:174_0": "Karissāmi pareti yo;",
  "anya-e1009n5:175_0": "Ajja kattabba kammaṁ sve,",
  "anya-e1009n5:176_0": "So tato parihāyati.",
  "anya-e1009n5:177_0": "39.",
  "anya-e1009n5:178_0": "So a-ppamatto a-kuddho,",
  "anya-e1009n5:179_0": "Tāta kiccāni kāraya;",
  "anya-e1009n5:180_0": "Vāyāmassu sa-kiccesu,",
  "anya-e1009n5:181_0": "Nālaso vindate sukhaṁ.",
  "anya-e1009n5:182_0": "40.",
  "anya-e1009n5:183_0": "Hīna-jaccopi ce hoti,",
  "anya-e1009n5:184_0": "Uṭṭhātā dhitimā naro;",
  "anya-e1009n5:185_0": "Ācāra-sīla-sampanno,",
  "anya-e1009n5:186_0": "Nise aggīva bhāsati.",
  "anya-e1009n5:187_0": "41.",
  "anya-e1009n5:188_0": "Yo pubbe karaṇīyāni,",
  "anya-e1009n5:189_0": "Pacchā so kātumicchati;",
  "anya-e1009n5:190_0": "Varuṇakaṭṭhabhañjova,",
  "anya-e1009n5:191_0": "Sa pacchā anutappati.",
  "anya-e1009n5:192_0": "42.",
  "anya-e1009n5:193_0": "Uṭṭhānakālamhi anuṭṭhahāno,",
  "anya-e1009n5:194_0": "Yuvā balī ālasiyaṁ upeto;",
  "anya-e1009n5:195_0": "A-puṇṇasaṅkappamano kusīto,",
  "anya-e1009n5:196_0": "Paññāya maggaṁ alaso na vindati.",
  "anya-e1009n5:197_0": "43.",
  "anya-e1009n5:198_0": "Dummedho puriso loke,",
  "anya-e1009n5:199_0": "Kusīto hīna-vīriyo;",
  "anya-e1009n5:200_0": "Appassuto anācāro,",
  "anya-e1009n5:201_0": "Parihāyati vuḍḍhiyā.",
  "anya-e1009n5:202_0": "44.",
  "anya-e1009n5:203_0": "Appassutāyaṁ puriso,",
  "anya-e1009n5:204_0": "Balībaddova jīrati;",
  "anya-e1009n5:205_0": "Maṁsāni tassa vaḍḍhanti,",
  "anya-e1009n5:206_0": "Paññā tassa na vaḍḍhati.",
  "anya-e1009n5:207_0": "45.",
  "anya-e1009n5:208_0": "Yassa manussa-bhūtassa,",
  "anya-e1009n5:209_0": "Natthi bhogā ca sippakaṁ;",
  "anya-e1009n5:210_0": "Kiṁphalaṁ tassa mānussaṁ,",
  "anya-e1009n5:211_0": "Dvipādaṭṭho hi so migo.",
  "anya-e1009n5:212_0": "46.",
  "anya-e1009n5:213_0": "Yo ca vassasataṁ jīve,",
  "anya-e1009n5:214_0": "Kusīto hīna-vīriyo;",
  "anya-e1009n5:215_0": "Ekāhaṁ jīvitaṁ seyyo,",
  "anya-e1009n5:216_0": "Vīriyārabbhato daḷaṁ.",
  "anya-e1009n5:217_0": "47.",
  "anya-e1009n5:218_0": "Yo ca dhamma-vibhaṅgaññū,",
  "anya-e1009n5:219_0": "Kāluṭṭhāyī a-tandito;",
  "anya-e1009n5:220_0": "Anuṭṭhahati kālena,",
  "anya-e1009n5:221_0": "Phalaṁ tassa samijjhati.",
  "anya-e1009n5:222_0": "48.",
  "anya-e1009n5:223_0": "A-caritvā brahmacariyaṁ,",
  "anya-e1009n5:224_0": "A-laddhā yobbane dhanaṁ;",
  "anya-e1009n5:225_0": "Jiṇṇakoñcāva jhāyanti,",
  "anya-e1009n5:226_0": "Khīṇamaccheva pallale.",
  "anya-e1009n5:227_0": "49.",
  "anya-e1009n5:228_0": "A-caritvā brahmacariyaṁ,",
  "anya-e1009n5:229_0": "A-laddhā yobbane dhanaṁ;",
  "anya-e1009n5:230_0": "Sentti cāpātikhīṇāva,",
  "anya-e1009n5:231_0": "Purāṇāni anutthunaṁ.",
  "anya-e1009n5:232_0": "50.",
  "anya-e1009n5:233_0": "Appakenāpi medhāvī,",
  "anya-e1009n5:234_0": "Pābhatena vicakkhaṇo;",
  "anya-e1009n5:235_0": "Samuṭṭhāpeti attānaṁ,",
  "anya-e1009n5:236_0": "Aṇuṁ aggiṁva sandhamaṁ.",
  "anya-e1009n5:237_0": "51.",
  "anya-e1009n5:238_0": "Vāyāmetheva puriso,",
  "anya-e1009n5:239_0": "Na nibbindeyya paṇḍito;",
  "anya-e1009n5:240_0": "Vāyāmassa phalaṁ passa,",
  "anya-e1009n5:241_0": "Bhuttā ambā anītihaṁ.",
  "anya-e1009n5:242_0": "52.",
  "anya-e1009n5:243_0": "Anuṭṭhahaṁ a-vāyāmaṁ,",
  "anya-e1009n5:244_0": "Sukhaṁ yatrādhi gacchati;",
  "anya-e1009n5:245_0": "Suvira tattha gacchāhi,",
  "anya-e1009n5:246_0": "Mañca tattheva pāpaya.",
  "anya-e1009n5:247_0": "53.",
  "anya-e1009n5:248_0": "Yatthālaso anuṭṭhātā,",
  "anya-e1009n5:249_0": "Accantaṁ sukhamedhati;",
  "anya-e1009n5:250_0": "Suvira tattha gacchāhi,",
  "anya-e1009n5:251_0": "Mañca ttattheva pāpaya.",
  "anya-e1009n5:252_0": "54.",
  "anya-e1009n5:253_0": "Adhippāya-phalaṁ loke,",
  "anya-e1009n5:254_0": "Dhītimanttassa sijjhati;",
  "anya-e1009n5:255_0": "Vīriyameva kattabbaṁ,",
  "anya-e1009n5:256_0": "Etaṁ buddhehi vaṇṇitaṁ."
}