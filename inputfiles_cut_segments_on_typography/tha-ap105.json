{
  "tha-ap105:0.1_0": "Therāpadāna",
  "tha-ap105:0.2_0": "Bhikkhadāyivagga",
  "tha-ap105:0.3_0": "3. Uppalahatthiyattheraapadāna",
  "tha-ap105:1.1_0": "“Tivarāyaṁ nivāsīhaṁ,",
  "tha-ap105:1.2_0": "ahosiṁ māliko tadā;",
  "tha-ap105:1.3_0": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap105:1.4_0": "siddhatthaṁ lokapūjitaṁ.",
  "tha-ap105:2.1_0": "Pasannacitto sumano,",
  "tha-ap105:2.2_0": "pupphahatthamadāsahaṁ;",
  "tha-ap105:2.3_0": "Yattha yatthupapajjāmi,",
  "tha-ap105:2.4_0": "tassa kammassa vāhasā.",
  "tha-ap105:3.1_0": "Anubhomi phalaṁ iṭṭhaṁ,",
  "tha-ap105:3.2_0": "pubbe sukatamattano;",
  "tha-ap105:3.3_0": "Parikkhitto sumallehi,",
  "tha-ap105:3.4_0": "pupphadānassidaṁ phalaṁ.",
  "tha-ap105:4.1_0": "Catunnavutito kappe,",
  "tha-ap105:4.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap105:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap105:4.4_0": "pupphapūjāyidaṁ phalaṁ.",
  "tha-ap105:5.1_0": "Catunnavutupādāya,",
  "tha-ap105:5.2_0": "ṭhapetvā vattamānakaṁ;",
  "tha-ap105:5.3_0": "Pañcarājasatā tattha,",
  "tha-ap105:5.4_0": "najjasamasanāmakā.",
  "tha-ap105:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap105:6.2_0": "…pe…",
  "tha-ap105:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap105:7.1_0": "Itthaṁ sudaṁ āyasmā uppalahatthiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap105:8.1_0": "Uppalahatthiyattherassāpadānaṁ tatiyaṁ."
}