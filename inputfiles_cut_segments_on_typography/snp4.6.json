{
  "snp4.6:0.1_0": "Sutta Nipāta 4.6",
  "snp4.6:0.2_0": "Jarāsutta",
  "snp4.6:1.1_0": "Appaṁ vata jīvitaṁ idaṁ,",
  "snp4.6:1.2_0": "Oraṁ vassasatāpi miyyati;",
  "snp4.6:1.3_0": "Yo cepi aticca jīvati,",
  "snp4.6:1.4_0": "Atha kho so jarasāpi miyyati.",
  "snp4.6:2.1_0": "Socanti janā mamāyite,",
  "snp4.6:2.2_0": "Na hi santi niccā pariggahā;",
  "snp4.6:2.3_0": "Vinābhāvasantamevidaṁ,",
  "snp4.6:2.4_0": "Iti disvā nāgāramāvase.",
  "snp4.6:3.1_0": "Maraṇenapi taṁ pahīyati,",
  "snp4.6:3.2_0": "Yaṁ puriso mamidanti maññati;",
  "snp4.6:3.3_0": "Etampi viditvā paṇḍito,",
  "snp4.6:3.4_0": "Na mamattāya nametha māmako.",
  "snp4.6:4.1_0": "Supinena yathāpi saṅgataṁ,",
  "snp4.6:4.2_0": "Paṭibuddho puriso na passati;",
  "snp4.6:4.3_0": "Evampi piyāyitaṁ janaṁ,",
  "snp4.6:4.4_0": "Petaṁ kālaṅkataṁ na passati.",
  "snp4.6:5.1_0": "Diṭṭhāpi sutāpi te janā,",
  "snp4.6:5.2_0": "Yesaṁ nāmamidaṁ pavuccati;",
  "snp4.6:5.3_0": "Nāmaṁyevāvasissati,",
  "snp4.6:5.4_0": "Akkheyyaṁ petassa jantuno.",
  "snp4.6:6.1_0": "Sokapparidevamaccharaṁ,",
  "snp4.6:6.2_0": "Na jahanti giddhā mamāyite;",
  "snp4.6:6.3_0": "Tasmā munayo pariggahaṁ,",
  "snp4.6:6.4_0": "Hitvā acariṁsu khemadassino.",
  "snp4.6:7.1_0": "Patilīnacarassa bhikkhuno,",
  "snp4.6:7.2_0": "Bhajamānassa vivittamāsanaṁ;",
  "snp4.6:7.3_0": "Sāmaggiyamāhu tassa taṁ,",
  "snp4.6:7.4_0": "Yo attānaṁ bhavane na dassaye.",
  "snp4.6:8.1_0": "Sabbattha munī anissito,",
  "snp4.6:8.2_0": "Na piyaṁ kubbati nopi appiyaṁ;",
  "snp4.6:8.3_0": "Tasmiṁ paridevamaccharaṁ,",
  "snp4.6:8.4_0": "Paṇṇe vāri yathā na limpati.",
  "snp4.6:9.1_0": "Udabindu yathāpi pokkhare,",
  "snp4.6:9.2_0": "Padume vāri yathā na limpati;",
  "snp4.6:9.3_0": "Evaṁ muni nopalimpati,",
  "snp4.6:9.4_0": "Yadidaṁ diṭṭhasutaṁ mutesu vā.",
  "snp4.6:10.1_0": "Dhono na hi tena maññati,",
  "snp4.6:10.2_0": "Yadidaṁ diṭṭhasutaṁ mutesu vā;",
  "snp4.6:10.3_0": "Nāññena visuddhimicchati,",
  "snp4.6:10.4_0": "Na hi so rajjati no virajjatīti.",
  "snp4.6:10.5_0": "Jarāsuttaṁ chaṭṭhaṁ."
}