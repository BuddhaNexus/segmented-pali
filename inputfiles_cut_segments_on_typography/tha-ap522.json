{
  "tha-ap522:0.1_0": "Therāpadāna",
  "tha-ap522:0.2_0": "Phaladāyakavagga",
  "tha-ap522:0.3_0": "10 Padumadhārikattheraapadāna",
  "tha-ap522:1.1_0": "“Himavantassāvidūre,",
  "tha-ap522:1.2_0": "romaso nāma pabbato;",
  "tha-ap522:1.3_0": "Buddhopi sambhavo nāma,",
  "tha-ap522:1.4_0": "abbhokāse vasī tadā.",
  "tha-ap522:2.1_0": "Bhavanā nikkhamitvāna,",
  "tha-ap522:2.2_0": "padumaṁ dhārayiṁ ahaṁ;",
  "tha-ap522:2.3_0": "Ekāhaṁ dhārayitvāna,",
  "tha-ap522:2.4_0": "bhavanaṁ punarāgamiṁ.",
  "tha-ap522:3.1_0": "Ekatiṁse ito kappe,",
  "tha-ap522:3.2_0": "yaṁ buddhamabhipūjayiṁ;",
  "tha-ap522:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap522:3.4_0": "buddhapūjāyidaṁ phalaṁ.",
  "tha-ap522:4.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap522:4.2_0": "…pe…",
  "tha-ap522:4.3_0": "viharāmi anāsavo.",
  "tha-ap522:5.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap522:5.2_0": "…pe…",
  "tha-ap522:5.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap522:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap522:6.2_0": "…pe…",
  "tha-ap522:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap522:7.1_0": "Itthaṁ sudaṁ āyasmā padumadhāriko thero imā gāthāyo abhāsitthāti.",
  "tha-ap522:8.1_0": "Padumadhārikattherassāpadānaṁ dasamaṁ.",
  "tha-ap522:9.1_0": "Phaladāyakavaggo dvepaññāsamo.",
  "tha-ap522:10.1_0": "Tassuddānaṁ",
  "tha-ap522:11.1_0": "Kurañciyaṁ kapitthañca,",
  "tha-ap522:11.2_0": "kosambamatha ketakaṁ;",
  "tha-ap522:11.3_0": "Nāgapupphajjunañceva,",
  "tha-ap522:11.4_0": "kuṭajī ghosasaññako.",
  "tha-ap522:12.1_0": "Thero ca sabbaphalado,",
  "tha-ap522:12.2_0": "tathā padumadhāriko;",
  "tha-ap522:12.3_0": "Asīti cettha gāthāyo,",
  "tha-ap522:12.4_0": "tisso gāthā taduttari."
}