{
  "ja383:0.1_0": "Jātaka",
  "ja383:0.2_0": "Chakkanipāta",
  "ja383:0.3_0": "Avāriyavagga",
  "ja383:0.4_0": "8. Kukkuṭajātaka",
  "ja383:1.1_0": "“Sucittapattachadana,",
  "ja383:1.2_0": "tambacūḷa vihaṅgama;",
  "ja383:1.3_0": "Oroha dumasākhāya,",
  "ja383:1.4_0": "mudhā bhariyā bhavāmi te”.",
  "ja383:2.1_0": "“Catuppadī tvaṁ kalyāṇi,",
  "ja383:2.2_0": "dvipadāhaṁ manorame;",
  "ja383:2.3_0": "Migī pakkhī asaññuttā,",
  "ja383:2.4_0": "aññaṁ pariyesa sāmikaṁ”.",
  "ja383:3.1_0": "“Komārikā te hessāmi,",
  "ja383:3.2_0": "Mañjukā piyabhāṇinī;",
  "ja383:3.3_0": "Vinda maṁ ariyena vedena,",
  "ja383:3.4_0": "Sāvaya maṁ yadicchasi”.",
  "ja383:4.1_0": "“Kuṇapādini lohitape,",
  "ja383:4.2_0": "cori kukkuṭapothini;",
  "ja383:4.3_0": "Na tvaṁ ariyena vedena,",
  "ja383:4.4_0": "mamaṁ bhattāramicchasi”.",
  "ja383:5.1_0": "“Evampi caturā nārī,",
  "ja383:5.2_0": "disvāna sadhanaṁ naraṁ;",
  "ja383:5.3_0": "Nenti saṇhāhi vācāhi,",
  "ja383:5.4_0": "biḷārī viya kukkuṭaṁ.",
  "ja383:6.1_0": "Yo ca uppatitaṁ atthaṁ,",
  "ja383:6.2_0": "na khippamanubujjhati;",
  "ja383:6.3_0": "Amittavasamanveti,",
  "ja383:6.4_0": "pacchā ca anutappati.",
  "ja383:7.1_0": "Yo ca uppatitaṁ atthaṁ,",
  "ja383:7.2_0": "khippameva nibodhati;",
  "ja383:7.3_0": "Muccate sattusambādhā,",
  "ja383:7.4_0": "kukkuṭova biḷāriyā”ti.",
  "ja383:8.1_0": "Kukkuṭajātakaṁ aṭṭhamaṁ."
}