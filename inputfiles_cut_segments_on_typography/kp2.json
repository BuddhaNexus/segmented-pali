{
  "kp2:0.1_0": "Khuddakapāṭha 2",
  "kp2:0.2_0": "Dasasikkhāpada",
  "kp2:1.1_0": "Pāṇātipātā veramaṇisikkhāpadaṁ samādiyāmi.",
  "kp2:2.1_0": "Adinnādānā veramaṇisikkhāpadaṁ samādiyāmi.",
  "kp2:3.1_0": "Abrahmacariyā veramaṇisikkhāpadaṁ samādiyāmi.",
  "kp2:4.1_0": "Musāvādā veramaṇisikkhāpadaṁ samādiyāmi.",
  "kp2:5.1_0": "Surāmerayamajjapamādaṭṭhānā veramaṇisikkhāpadaṁ samādiyāmi.",
  "kp2:6.1_0": "Vikālabhojanā veramaṇisikkhāpadaṁ samādiyāmi.",
  "kp2:7.1_0": "Naccagītavāditavisūkadassanā veramaṇisikkhāpadaṁ samādiyāmi.",
  "kp2:8.1_0": "Mālāgandhavilepanadhāraṇamaṇḍanavibhūsanaṭṭhānā veramaṇisikkhāpadaṁ samādiyāmi.",
  "kp2:9.1_0": "Uccāsayanamahāsayanā veramaṇisikkhāpadaṁ samādiyāmi.",
  "kp2:10.1_0": "Jātarūparajatapaṭiggahaṇā veramaṇisikkhāpadaṁ samādiyāmi.",
  "kp2:11.1_0": "Dasasikkhāpadaṁ."
}