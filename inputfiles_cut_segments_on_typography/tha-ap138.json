{
  "tha-ap138:0.1_0": "Therāpadāna",
  "tha-ap138:0.2_0": "Sobhitavagga",
  "tha-ap138:0.3_0": "6. Campakapupphiyattheraapadāna",
  "tha-ap138:1.1_0": "“Kaṇikāraṁva jotantaṁ,",
  "tha-ap138:1.2_0": "nisinnaṁ pabbatantare;",
  "tha-ap138:1.3_0": "Obhāsentaṁ disā sabbā,",
  "tha-ap138:1.4_0": "osadhiṁ viya tārakaṁ.",
  "tha-ap138:2.1_0": "Tayo māṇavakā āsuṁ,",
  "tha-ap138:2.2_0": "sake sippe susikkhitā;",
  "tha-ap138:2.3_0": "Khāribhāraṁ gahetvāna,",
  "tha-ap138:2.4_0": "anventi mama pacchato.",
  "tha-ap138:3.1_0": "Puṭake satta pupphāni,",
  "tha-ap138:3.2_0": "nikkhittāni tapassinā;",
  "tha-ap138:3.3_0": "Gahetvā tāni ñāṇamhi,",
  "tha-ap138:3.4_0": "vessabhussābhiropayiṁ.",
  "tha-ap138:4.1_0": "Ekattiṁse ito kappe,",
  "tha-ap138:4.2_0": "yaṁ pupphamabhiropayiṁ;",
  "tha-ap138:4.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap138:4.4_0": "ñāṇapūjāyidaṁ phalaṁ.",
  "tha-ap138:5.1_0": "Ekūnatiṁsakappamhi,",
  "tha-ap138:5.2_0": "vipulābhasanāmako;",
  "tha-ap138:5.3_0": "Sattaratanasampanno,",
  "tha-ap138:5.4_0": "cakkavattī mahabbalo.",
  "tha-ap138:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap138:6.2_0": "…pe…",
  "tha-ap138:6.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap138:7.1_0": "Itthaṁ sudaṁ āyasmā campakapupphiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap138:8.1_0": "Campakapupphiyattherassāpadānaṁ chaṭṭhaṁ."
}