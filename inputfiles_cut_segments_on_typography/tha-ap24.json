{
  "tha-ap24:0.1_0": "Therāpadāna",
  "tha-ap24:0.2_0": "Subhūtivagga",
  "tha-ap24:0.3_0": "2 Upavānattheraapadāna",
  "tha-ap24:1.1_0": "“Padumuttaro nāma jino,",
  "tha-ap24:1.2_0": "sabbadhammāna pāragū;",
  "tha-ap24:1.3_0": "Jalitvā aggikkhandhova,",
  "tha-ap24:1.4_0": "sambuddho parinibbuto.",
  "tha-ap24:2.1_0": "Mahājanā samāgamma,",
  "tha-ap24:2.2_0": "pūjayitvā tathāgataṁ;",
  "tha-ap24:2.3_0": "Citaṁ katvāna sukataṁ,",
  "tha-ap24:2.4_0": "sarīraṁ abhiropayuṁ.",
  "tha-ap24:3.1_0": "Sarīrakiccaṁ katvāna,",
  "tha-ap24:3.2_0": "dhātuṁ tattha samānayuṁ;",
  "tha-ap24:3.3_0": "Sadevamānusā sabbe,",
  "tha-ap24:3.4_0": "buddhathūpaṁ akaṁsu te.",
  "tha-ap24:4.1_0": "Paṭhamā kañcanamayā,",
  "tha-ap24:4.2_0": "dutiyāsi maṇīmayā;",
  "tha-ap24:4.3_0": "Tatiyā rūpiyamayā,",
  "tha-ap24:4.4_0": "catutthī phalikāmayā.",
  "tha-ap24:5.1_0": "Tathā pañcamiyā bhūmi,",
  "tha-ap24:5.2_0": "lohitaṅgamayā ahu;",
  "tha-ap24:5.3_0": "Chaṭṭhā masāragallassa,",
  "tha-ap24:5.4_0": "sabbaratanamayūpari.",
  "tha-ap24:6.1_0": "Jaṅghā maṇimayā āsi,",
  "tha-ap24:6.2_0": "vedikā ratanamayā;",
  "tha-ap24:6.3_0": "Sabbasoṇṇamayo thūpo,",
  "tha-ap24:6.4_0": "uddhaṁ yojanamuggato.",
  "tha-ap24:7.1_0": "Devā tattha samāgantvā,",
  "tha-ap24:7.2_0": "ekato mantayuṁ tadā;",
  "tha-ap24:7.3_0": "Mayampi thūpaṁ kassāma,",
  "tha-ap24:7.4_0": "lokanāthassa tādino.",
  "tha-ap24:8.1_0": "Dhātu āveṇikā natthi,",
  "tha-ap24:8.2_0": "sarīraṁ ekapiṇḍitaṁ;",
  "tha-ap24:8.3_0": "Imamhi buddhathūpamhi,",
  "tha-ap24:8.4_0": "kassāma kañcukaṁ mayaṁ.",
  "tha-ap24:9.1_0": "Devā sattahi ratnehi,",
  "tha-ap24:9.2_0": "aññaṁ vaḍḍhesu yojanaṁ;",
  "tha-ap24:9.3_0": "Thūpo dviyojanubbedho,",
  "tha-ap24:9.4_0": "timiraṁ byapahanti so.",
  "tha-ap24:10.1_0": "Nāgā tattha samāgantvā,",
  "tha-ap24:10.2_0": "ekato mantayuṁ tadā;",
  "tha-ap24:10.3_0": "Manussā ceva devā ca,",
  "tha-ap24:10.4_0": "buddhathūpaṁ akaṁsu te.",
  "tha-ap24:11.1_0": "Mā no pamattā assumha,",
  "tha-ap24:11.2_0": "appamattā sadevakā;",
  "tha-ap24:11.3_0": "Mayampi thūpaṁ kassāma,",
  "tha-ap24:11.4_0": "lokanāthassa tādino.",
  "tha-ap24:12.1_0": "Indanīlaṁ mahānīlaṁ,",
  "tha-ap24:12.2_0": "atho jotirasaṁ maṇiṁ;",
  "tha-ap24:12.3_0": "Ekato sannipātetvā,",
  "tha-ap24:12.4_0": "buddhathūpaṁ achādayuṁ.",
  "tha-ap24:13.1_0": "Sabbaṁ maṇimayaṁ āsi,",
  "tha-ap24:13.2_0": "tāvatā buddhacetiyaṁ;",
  "tha-ap24:13.3_0": "Tiyojanasamubbiddhaṁ,",
  "tha-ap24:13.4_0": "ālokakaraṇaṁ tadā.",
  "tha-ap24:14.1_0": "Garuḷā ca samāgantvā,",
  "tha-ap24:14.2_0": "ekato mantayuṁ tadā;",
  "tha-ap24:14.3_0": "Manussā devā nāgā ca,",
  "tha-ap24:14.4_0": "buddhathūpaṁ akaṁsu te.",
  "tha-ap24:15.1_0": "‘Mā no pamattā assumha,",
  "tha-ap24:15.2_0": "appamattā sadevakā;",
  "tha-ap24:15.3_0": "Mayampi thūpaṁ kassāma,",
  "tha-ap24:15.4_0": "lokanāthassa tādino’.",
  "tha-ap24:16.1_0": "Sabbaṁ maṇimayaṁ thūpaṁ,",
  "tha-ap24:16.2_0": "akaruṁ te ca kañcukaṁ;",
  "tha-ap24:16.3_0": "Yojanaṁ tepi vaḍḍhesuṁ,",
  "tha-ap24:16.4_0": "āyataṁ buddhacetiyaṁ.",
  "tha-ap24:17.1_0": "Catuyojanamubbiddho,",
  "tha-ap24:17.2_0": "buddhathūpo virocati;",
  "tha-ap24:17.3_0": "Obhāseti disā sabbā,",
  "tha-ap24:17.4_0": "sataraṁsīva uggato.",
  "tha-ap24:18.1_0": "Kumbhaṇḍā ca samāgantvā,",
  "tha-ap24:18.2_0": "ekato mantayuṁ tadā;",
  "tha-ap24:18.3_0": "Manussā ceva devā ca,",
  "tha-ap24:18.4_0": "nāgā ca garuḷā tathā;",
  "tha-ap24:18.5_0": "Paccekaṁ buddhaseṭṭhassa,",
  "tha-ap24:18.6_0": "akaṁsu thūpamuttamaṁ.",
  "tha-ap24:19.1_0": "‘Mā no pamattā assumha,",
  "tha-ap24:19.2_0": "appamattā sadevakā;",
  "tha-ap24:19.3_0": "Mayampi thūpaṁ kassāma,",
  "tha-ap24:19.4_0": "lokanāthassa tādino;",
  "tha-ap24:19.5_0": "Ratanehi chādessāma,",
  "tha-ap24:19.6_0": "āyataṁ buddhacetiyaṁ’.",
  "tha-ap24:20.1_0": "Yojanaṁ tepi vaḍḍhesuṁ,",
  "tha-ap24:20.2_0": "āyataṁ buddhacetiyaṁ;",
  "tha-ap24:20.3_0": "Pañcayojanamubbiddho,",
  "tha-ap24:20.4_0": "thūpo obhāsate tadā.",
  "tha-ap24:21.1_0": "Yakkhā tattha samāgantvā,",
  "tha-ap24:21.2_0": "ekato mantayuṁ tadā;",
  "tha-ap24:21.3_0": "Manussā devā nāgā ca,",
  "tha-ap24:21.4_0": "garuḷā kumbhaaṇḍakā.",
  "tha-ap24:22.1_0": "Paccekaṁ buddhaseṭṭhassa,",
  "tha-ap24:22.2_0": "akaṁsu thūpamuttamaṁ;",
  "tha-ap24:22.3_0": "‘Mā no pamattā assumha,",
  "tha-ap24:22.4_0": "appamattā sadevakā.",
  "tha-ap24:23.1_0": "Mayampi thūpaṁ kassāma,",
  "tha-ap24:23.2_0": "lokanāthassa tādino;",
  "tha-ap24:23.3_0": "Phalikāhi chādessāma,",
  "tha-ap24:23.4_0": "āyataṁ buddhacetiyaṁ’.",
  "tha-ap24:24.1_0": "Yojanaṁ tepi vaḍḍhesuṁ,",
  "tha-ap24:24.2_0": "āyataṁ buddhacetiyaṁ;",
  "tha-ap24:24.3_0": "Cha yojanāni ubbiddho,",
  "tha-ap24:24.4_0": "thūpo obhāsate tadā.",
  "tha-ap24:25.1_0": "Gandhabbā ca samāgantvā,",
  "tha-ap24:25.2_0": "ekato mantayuṁ tadā;",
  "tha-ap24:25.3_0": "‘Manujā devatā nāgā,",
  "tha-ap24:25.4_0": "garuḷā kumbhayakkhakā.",
  "tha-ap24:26.1_0": "Sabbekaṁsu buddhathūpaṁ,",
  "tha-ap24:26.2_0": "mayamettha akārakā;",
  "tha-ap24:26.3_0": "Mayampi thūpaṁ kassāma,",
  "tha-ap24:26.4_0": "lokanāthassa tādino’.",
  "tha-ap24:27.1_0": "Vediyo satta katvāna,",
  "tha-ap24:27.2_0": "chattamāropayiṁsu te;",
  "tha-ap24:27.3_0": "Sabbasoṇṇamayaṁ thūpaṁ,",
  "tha-ap24:27.4_0": "gandhabbā kārayuṁ tadā.",
  "tha-ap24:28.1_0": "Sattayojanamubbiddho,",
  "tha-ap24:28.2_0": "thūpo obhāsate tadā;",
  "tha-ap24:28.3_0": "Rattindivā na ñāyanti,",
  "tha-ap24:28.4_0": "āloko hoti sabbadā.",
  "tha-ap24:29.1_0": "Abhibhonti na tassābhā,",
  "tha-ap24:29.2_0": "candasūrā satārakā;",
  "tha-ap24:29.3_0": "Samantā yojanasate,",
  "tha-ap24:29.4_0": "padīpopi na pajjali.",
  "tha-ap24:30.1_0": "Tena kālena ye keci,",
  "tha-ap24:30.2_0": "thūpaṁ pūjenti mānusā;",
  "tha-ap24:30.3_0": "Na te thūpamāruhanti,",
  "tha-ap24:30.4_0": "ambare ukkhipanti te.",
  "tha-ap24:31.1_0": "Devehi ṭhapito yakkho,",
  "tha-ap24:31.2_0": "abhisammatanāmako;",
  "tha-ap24:31.3_0": "Dhajaṁ vā pupphadāmaṁ vā,",
  "tha-ap24:31.4_0": "abhiropeti uttari.",
  "tha-ap24:32.1_0": "Na te passanti taṁ yakkhaṁ,",
  "tha-ap24:32.2_0": "dāmaṁ passanti gacchato;",
  "tha-ap24:32.3_0": "Evaṁ passitvā gacchantā,",
  "tha-ap24:32.4_0": "sabbe gacchanti suggatiṁ.",
  "tha-ap24:33.1_0": "Viruddhā ye pāvacane,",
  "tha-ap24:33.2_0": "pasannā ye ca sāsane;",
  "tha-ap24:33.3_0": "Pāṭiheraṁ daṭṭhukāmā,",
  "tha-ap24:33.4_0": "thūpaṁ pūjenti mānusā.",
  "tha-ap24:34.1_0": "Nagare haṁsavatiyā,",
  "tha-ap24:34.2_0": "ahosiṁ bhatako tadā;",
  "tha-ap24:34.3_0": "Āmoditaṁ janaṁ disvā,",
  "tha-ap24:34.4_0": "evaṁ cintesahaṁ tadā.",
  "tha-ap24:35.1_0": "‘Uḷāro bhagavā heso,",
  "tha-ap24:35.2_0": "yassa dhātudharedisaṁ;",
  "tha-ap24:35.3_0": "Imā ca janatā tuṭṭhā,",
  "tha-ap24:35.4_0": "kāraṁ kubbaṁ na tappare.",
  "tha-ap24:36.1_0": "Ahampi kāraṁ kassāmi,",
  "tha-ap24:36.2_0": "lokanāthassa tādino;",
  "tha-ap24:36.3_0": "Tassa dhammesu dāyādo,",
  "tha-ap24:36.4_0": "bhavissāmi anāgate’.",
  "tha-ap24:37.1_0": "Sudhotaṁ rajakenāhaṁ,",
  "tha-ap24:37.2_0": "uttareyyapaṭaṁ mama;",
  "tha-ap24:37.3_0": "Veḷagge ālaggetvāna,",
  "tha-ap24:37.4_0": "dhajaṁ ukkhipimambare.",
  "tha-ap24:38.1_0": "Abhisammatako gayha,",
  "tha-ap24:38.2_0": "ambarehāsi me dhajaṁ;",
  "tha-ap24:38.3_0": "Vāteritaṁ dhajaṁ disvā,",
  "tha-ap24:38.4_0": "bhiyyo hāsaṁ janesahaṁ.",
  "tha-ap24:39.1_0": "Tattha cittaṁ pasādetvā,",
  "tha-ap24:39.2_0": "samaṇaṁ upasaṅkamiṁ;",
  "tha-ap24:39.3_0": "Taṁ bhikkhuṁ abhivādetvā,",
  "tha-ap24:39.4_0": "vipākaṁ pucchahaṁ dhaje.",
  "tha-ap24:40.1_0": "So me kathesi ānanda,",
  "tha-ap24:40.2_0": "pītisañjananaṁ mama;",
  "tha-ap24:40.3_0": "‘Tassa dhajassa vipākaṁ,",
  "tha-ap24:40.4_0": "anubhossasi sabbadā.",
  "tha-ap24:41.1_0": "Hatthī assā rathā pattī,",
  "tha-ap24:41.2_0": "senā ca caturaṅginī;",
  "tha-ap24:41.3_0": "Parivāressanti taṁ niccaṁ,",
  "tha-ap24:41.4_0": "dhajadānassidaṁ phalaṁ.",
  "tha-ap24:42.1_0": "Saṭṭhitūriyasahassāni,",
  "tha-ap24:42.2_0": "bheriyo samalaṅkatā;",
  "tha-ap24:42.3_0": "Parivāressanti taṁ niccaṁ,",
  "tha-ap24:42.4_0": "dhajadānassidaṁ phalaṁ.",
  "tha-ap24:43.1_0": "Chaḷāsītisahassāni,",
  "tha-ap24:43.2_0": "nāriyo samalaṅkatā;",
  "tha-ap24:43.3_0": "Vicittavatthābharaṇā,",
  "tha-ap24:43.4_0": "āmuttamaṇikuṇḍalā.",
  "tha-ap24:44.1_0": "Aḷārapamhā hasulā,",
  "tha-ap24:44.2_0": "susaññā tanumajjhimā;",
  "tha-ap24:44.3_0": "Parivāressanti taṁ niccaṁ,",
  "tha-ap24:44.4_0": "dhajadānassidaṁ phalaṁ.",
  "tha-ap24:45.1_0": "Tiṁsakappasahassāni,",
  "tha-ap24:45.2_0": "devaloke ramissasi;",
  "tha-ap24:45.3_0": "Asītikkhattuṁ devindo,",
  "tha-ap24:45.4_0": "devarajjaṁ karissasi.",
  "tha-ap24:46.1_0": "Sahassakkhattuṁ rājā ca,",
  "tha-ap24:46.2_0": "cakkavattī bhavissasi;",
  "tha-ap24:46.3_0": "Padesarajjaṁ vipulaṁ,",
  "tha-ap24:46.4_0": "gaṇanāto asaṅkhiyaṁ.",
  "tha-ap24:47.1_0": "Kappasatasahassamhi,",
  "tha-ap24:47.2_0": "okkākakulasambhavo;",
  "tha-ap24:47.3_0": "Gotamo nāma gottena,",
  "tha-ap24:47.4_0": "satthā loke bhavissati.",
  "tha-ap24:48.1_0": "Devalokā cavitvāna,",
  "tha-ap24:48.2_0": "sukkamūlena codito;",
  "tha-ap24:48.3_0": "Puññakammena saṁyutto,",
  "tha-ap24:48.4_0": "brahmabandhu bhavissasi.",
  "tha-ap24:49.1_0": "Asītikoṭiṁ chaḍḍetvā,",
  "tha-ap24:49.2_0": "dāse kammakare bahū;",
  "tha-ap24:49.3_0": "Gotamassa bhagavato,",
  "tha-ap24:49.4_0": "sāsane pabbajissasi.",
  "tha-ap24:50.1_0": "Ārādhayitvā sambuddhaṁ,",
  "tha-ap24:50.2_0": "gotamaṁ sakyapuṅgavaṁ;",
  "tha-ap24:50.3_0": "Upavānoti nāmena,",
  "tha-ap24:50.4_0": "hessasi satthu sāvako’.",
  "tha-ap24:51.1_0": "Satasahasse kataṁ kammaṁ,",
  "tha-ap24:51.2_0": "phalaṁ dassesi me idha;",
  "tha-ap24:51.3_0": "Sumutto saravegova,",
  "tha-ap24:51.4_0": "kilese jhāpayī mama.",
  "tha-ap24:52.1_0": "Cakkavattissa santassa,",
  "tha-ap24:52.2_0": "catudīpissarassa me;",
  "tha-ap24:52.3_0": "Tiyojanāni samantā,",
  "tha-ap24:52.4_0": "ussīsanti dhajā sadā.",
  "tha-ap24:53.1_0": "Satasahassito kappe,",
  "tha-ap24:53.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap24:53.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap24:53.4_0": "dhajadānassidaṁ phalaṁ.",
  "tha-ap24:54.1_0": "Paṭisambhidā catasso,",
  "tha-ap24:54.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap24:54.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap24:54.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap24:55.1_0": "Itthaṁ sudaṁ āyasmā upavāno thero imā gāthāyo abhāsitthāti.",
  "tha-ap24:56.1_0": "Upavānattherassāpadānaṁ dutiyaṁ."
}