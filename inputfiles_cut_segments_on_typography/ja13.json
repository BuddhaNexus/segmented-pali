{
  "ja13:0.1_0": "Jātaka",
  "ja13:0.2_0": "Ekakanipāta",
  "ja13:0.3_0": "Sīlavagga",
  "ja13:0.4_0": "3. Kaṇḍijātaka",
  "ja13:1.1_0": "“Dhiratthu kaṇḍinaṁ sallaṁ,",
  "ja13:1.2_0": "purisaṁ gāḷhavedhinaṁ;",
  "ja13:1.3_0": "Dhiratthu taṁ janapadaṁ,",
  "ja13:1.4_0": "yatthitthī pariṇāyikā;",
  "ja13:1.5_0": "Te cāpi dhikkitā sattā,",
  "ja13:1.6_0": "ye itthīnaṁ vasaṅgatā”ti.",
  "ja13:2.1_0": "Kaṇḍijātakaṁ tatiyaṁ."
}