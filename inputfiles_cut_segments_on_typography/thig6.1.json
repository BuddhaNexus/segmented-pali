{
  "thig6.1:0.1_0": "Therīgāthā 6.1",
  "thig6.1:0.2_0": "Chakkanipāta",
  "thig6.1:0.3_0": "Pañcasatamattātherīgāthā",
  "thig6.1:1.1_0": "“Yassa maggaṁ na jānāsi,",
  "thig6.1:1.2_0": "āgatassa gatassa vā;",
  "thig6.1:1.3_0": "Taṁ kuto cāgataṁ sattaṁ,",
  "thig6.1:1.4_0": "‘mama putto’ti rodasi.",
  "thig6.1:2.1_0": "Maggañca khossa jānāsi,",
  "thig6.1:2.2_0": "āgatassa gatassa vā;",
  "thig6.1:2.3_0": "Na naṁ samanusocesi,",
  "thig6.1:2.4_0": "evaṁdhammā hi pāṇino.",
  "thig6.1:3.1_0": "Ayācito tatāgacchi,",
  "thig6.1:3.2_0": "nānuññāto ito gato;",
  "thig6.1:3.3_0": "Kutoci nūna āgantvā,",
  "thig6.1:3.4_0": "vasitvā katipāhakaṁ;",
  "thig6.1:3.5_0": "Itopi aññena gato,",
  "thig6.1:3.6_0": "tatopaññena gacchati.",
  "thig6.1:4.1_0": "Peto manussarūpena,",
  "thig6.1:4.2_0": "saṁsaranto gamissati;",
  "thig6.1:4.3_0": "Yathāgato tathā gato,",
  "thig6.1:4.4_0": "kā tattha paridevanā”.",
  "thig6.1:5.1_0": "“Abbahī vata me sallaṁ,",
  "thig6.1:5.2_0": "duddasaṁ hadayassitaṁ;",
  "thig6.1:5.3_0": "Yā me sokaparetāya,",
  "thig6.1:5.4_0": "puttasokaṁ byapānudi.",
  "thig6.1:6.1_0": "Sājja abbūḷhasallāhaṁ,",
  "thig6.1:6.2_0": "Nicchātā parinibbutā;",
  "thig6.1:6.3_0": "Buddhaṁ dhammañca saṅghañca,",
  "thig6.1:6.4_0": "Upemi saraṇaṁ muniṁ”.",
  "thig6.1:7.1_0": "Itthaṁ sudaṁ pañcasatamattā therī bhikkhuniyo … pe…."
}