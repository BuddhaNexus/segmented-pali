{
  "tha-ap465:0.1_0": "Therāpadāna",
  "tha-ap465:0.2_0": "Sālakusumiyavagga",
  "tha-ap465:0.3_0": "3. Citakanibbāpakattheraapadāna",
  "tha-ap465:1.1_0": "“Dayhamāne sarīramhi,",
  "tha-ap465:1.2_0": "vessabhussa mahesino;",
  "tha-ap465:1.3_0": "Gandhodakaṁ gahetvāna,",
  "tha-ap465:1.4_0": "citaṁ nibbāpayiṁ ahaṁ.",
  "tha-ap465:2.1_0": "Ekatiṁse ito kappe,",
  "tha-ap465:2.2_0": "citaṁ nibbāpayiṁ ahaṁ;",
  "tha-ap465:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap465:2.4_0": "gandhodakassidaṁ phalaṁ.",
  "tha-ap465:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap465:3.2_0": "…pe…",
  "tha-ap465:3.3_0": "viharāmi anāsavo.",
  "tha-ap465:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap465:4.2_0": "…pe…",
  "tha-ap465:4.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap465:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap465:5.2_0": "…pe…",
  "tha-ap465:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap465:6.1_0": "Itthaṁ sudaṁ āyasmā citakanibbāpako thero imā gāthāyo abhāsitthāti.",
  "tha-ap465:7.1_0": "Citakanibbāpakattherassāpadānaṁ tatiyaṁ."
}