{
  "ja58:0.1_0": "Jātaka",
  "ja58:0.2_0": "Ekakanipāta",
  "ja58:0.3_0": "Āsīsavagga",
  "ja58:0.4_0": "8. Tayodhammajātaka",
  "ja58:1.1_0": "“Yassa ete tayo dhammā,",
  "ja58:1.2_0": "vānarinda yathā tava;",
  "ja58:1.3_0": "Dakkhiyaṁ sūriyaṁ paññā,",
  "ja58:1.4_0": "diṭṭhaṁ so ativattatī”ti.",
  "ja58:2.1_0": "Tayodhammajātakaṁ aṭṭhamaṁ."
}