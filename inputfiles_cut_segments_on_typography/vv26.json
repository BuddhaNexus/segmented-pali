{
  "vv26:0.1_0": "Vimānavatthu",
  "vv26:0.2_0": "Itthivimāna",
  "vv26:0.3_0": "Cittalatāvagga",
  "vv26:0.4_0": "9. Suniddāvimānavatthu",
  "vv26:1.1_0": "“Abhikkantena vaṇṇena,",
  "vv26:1.2_0": "…pe…",
  "vv26:1.3_0": "osadhī viya tārakā.",
  "vv26:2.1_0": "Kena tetādiso vaṇṇo,",
  "vv26:2.2_0": "…pe…",
  "vv26:2.3_0": "vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv26:3.1_0": "Sā devatā attamanā,",
  "vv26:3.2_0": "…pe…",
  "vv26:3.3_0": "yassa kammassidaṁ phalaṁ.",
  "vv26:4.1_0": "“Suniddāti maṁ aññaṁsu,",
  "vv26:4.2_0": "rājagahasmiṁ upāsikā;",
  "vv26:4.3_0": "Saddhā sīlena sampannā,",
  "vv26:4.4_0": "saṁvibhāgaratā sadā.",
  "vv26:5.1_0": "(…)",
  "vv26:6.1_0": "Pañcasikkhāpade ratā,",
  "vv26:6.2_0": "ariyasaccāna kovidā;",
  "vv26:6.3_0": "Upāsikā cakkhumato,",
  "vv26:6.4_0": "gotamassa yasassino.",
  "vv26:7.1_0": "Tena metādiso vaṇṇo,",
  "vv26:7.2_0": "…pe…",
  "vv26:7.3_0": "vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv26:8.1_0": "Suniddāvimānaṁ navamaṁ."
}