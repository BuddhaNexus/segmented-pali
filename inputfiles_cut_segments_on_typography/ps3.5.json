{
  "ps3.5:0.1_0": "Paṭisambhidāmagga",
  "ps3.5:0.2_0": "3 Paññāvagga",
  "ps3.5:0.3_0": "3.5. Cariyākathā",
  "ps3.5:1.1_0": "<b>Cariyā</b>ti, aṭṭha cariyāyo—",
  "ps3.5:1.2_0": "iriyāpathacariyā,",
  "ps3.5:1.2_1": "āyatanacariyā,",
  "ps3.5:1.2_2": "saticariyā,",
  "ps3.5:1.2_3": "samādhicariyā,",
  "ps3.5:1.2_4": "ñāṇacariyā,",
  "ps3.5:1.2_5": "maggacariyā,",
  "ps3.5:1.2_6": "patticariyā,",
  "ps3.5:1.2_7": "lokatthacariyāti.",
  "ps3.5:2.1_0": "<b>Iriyāpathacariyā</b>ti catūsu iriyāpathesu.",
  "ps3.5:2.2_0": "<b>Āyatanacariyā</b>ti chasu ajjhattikabāhiresu āyatanesu.",
  "ps3.5:2.3_0": "<b>Saticariyā</b>ti catūsu satipaṭṭhānesu.",
  "ps3.5:2.4_0": "<b>Samādhicariyā</b>ti catūsu jhānesu.",
  "ps3.5:2.5_0": "<b>Ñāṇacariyā</b>ti catūsu ariyasaccesu.",
  "ps3.5:2.6_0": "<b>Maggacariyā</b>ti catūsu ariyamaggesu.",
  "ps3.5:2.7_0": "<b>Patticariyā</b>ti catūsu sāmaññaphalesu.",
  "ps3.5:2.8_0": "<b>Lokatthacariyā</b>ti tathāgatesu arahantesu sammāsambuddhesu padese paccekabuddhesu padese sāvakesu.",
  "ps3.5:3.1_0": "Iriyāpathacariyā ca paṇidhisampannānaṁ.",
  "ps3.5:3.2_0": "Āyatanacariyā ca indriyesu guttadvārānaṁ.",
  "ps3.5:3.3_0": "Saticariyā ca appamādavihārīnaṁ.",
  "ps3.5:3.4_0": "Samādhicariyā ca adhicittamanuyuttānaṁ.",
  "ps3.5:3.5_0": "Ñāṇacariyā ca buddhisampannānaṁ.",
  "ps3.5:3.6_0": "Maggacariyā ca sammāpaṭipannānaṁ.",
  "ps3.5:3.7_0": "Patticariyā ca adhigataphalānaṁ.",
  "ps3.5:3.8_0": "Lokatthacariyā ca tathāgatānaṁ arahantānaṁ sammāsambuddhānaṁ padese paccekabuddhānaṁ padese sāvakānaṁ.",
  "ps3.5:3.9_0": "Imā aṭṭha cariyāyo.",
  "ps3.5:4.1_0": "Aparāpi aṭṭha cariyāyo.",
  "ps3.5:4.2_0": "Adhimuccanto saddhāya carati,",
  "ps3.5:4.2_1": "paggaṇhanto vīriyena carati,",
  "ps3.5:4.2_2": "upaṭṭhāpento satiyā carati,",
  "ps3.5:4.2_3": "avikkhepaṁ karonto samādhinā carati,",
  "ps3.5:4.2_4": "pajānanto paññāya carati,",
  "ps3.5:4.2_5": "vijānanto viññāṇacariyāya carati,",
  "ps3.5:4.2_6": "evaṁ paṭipannassa kusalā dhammā āyāpentīti āyatanacariyāya carati,",
  "ps3.5:4.2_7": "evaṁ paṭipanno visesamadhigacchatīti visesacariyāya carati.",
  "ps3.5:4.3_0": "Imā aṭṭha cariyāyo.",
  "ps3.5:5.1_0": "Aparāpi aṭṭha cariyāyo.",
  "ps3.5:5.2_0": "Dassanacariyā ca sammādiṭṭhiyā,",
  "ps3.5:5.2_1": "abhiniropanacariyā ca sammāsaṅkappassa,",
  "ps3.5:5.2_2": "pariggahacariyā ca sammāvācāya,",
  "ps3.5:5.2_3": "samuṭṭhānacariyā ca sammākammantassa,",
  "ps3.5:5.2_4": "vodānacariyā ca sammāājīvassa,",
  "ps3.5:5.2_5": "paggahacariyā ca sammāvāyāmassa,",
  "ps3.5:5.2_6": "upaṭṭhānacariyā ca sammāsatiyā,",
  "ps3.5:5.2_7": "avikkhepacariyā ca sammāsamādhissa.",
  "ps3.5:5.3_0": "Imā aṭṭha cariyāyoti.",
  "ps3.5:5.4_0": "Cariyākathā niṭṭhitā."
}