{
  "thag1.83:0.1_0": "Theragāthā 1.83",
  "thag1.83:0.2_0": "Ekakanipāta",
  "thag1.83:0.3_0": "Navamavagga",
  "thag1.83:0.4_0": "Sīhattheragāthā",
  "thag1.83:1.1_0": "“Sīhappamatto vihara,",
  "thag1.83:1.2_0": "rattindivamatandito;",
  "thag1.83:1.3_0": "Bhāvehi kusalaṁ dhammaṁ,",
  "thag1.83:1.4_0": "jaha sīghaṁ samussayan”ti.",
  "thag1.83:1.5_0": "… Sīho thero …."
}