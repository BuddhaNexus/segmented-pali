{
  "iti56:0.1_0": "Itivuttaka 56",
  "iti56:0.2_0": "Tikanipāta",
  "iti56:0.3_0": "Paṭhamavagga",
  "iti56:0.4_0": "Paṭhamaāsavasutta",
  "iti56:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti56:2.1_0": "“Tayome, bhikkhave, āsavā.",
  "iti56:2.2_0": "Katame tayo?",
  "iti56:2.3_0": "Kāmāsavo, bhavāsavo, avijjāsavo—",
  "iti56:2.4_0": "ime kho, bhikkhave, tayo āsavā”ti.",
  "iti56:2.5_0": "Etamatthaṁ bhagavā avoca.",
  "iti56:2.6_0": "Tatthetaṁ iti vuccati:",
  "iti56:3.1_0": "“Samāhito sampajāno,",
  "iti56:3.2_0": "sato buddhassa sāvako;",
  "iti56:3.3_0": "Āsave ca pajānāti,",
  "iti56:3.4_0": "āsavānañca sambhavaṁ.",
  "iti56:4.1_0": "Yattha cetā nirujjhanti,",
  "iti56:4.2_0": "maggañca khayagāminaṁ;",
  "iti56:4.3_0": "Āsavānaṁ khayā bhikkhu,",
  "iti56:4.4_0": "nicchāto parinibbuto”ti.",
  "iti56:5.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti56:5.2_0": "Sattamaṁ."
}