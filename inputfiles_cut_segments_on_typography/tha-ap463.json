{
  "tha-ap463:0.1_0": "Therāpadāna",
  "tha-ap463:0.2_0": "Sālakusumiyavagga",
  "tha-ap463:0.3_0": "1. Sālakusumiyattheraapadāna",
  "tha-ap463:1.1_0": "“Parinibbute bhagavati,",
  "tha-ap463:1.2_0": "jalajuttamanāmake;",
  "tha-ap463:1.3_0": "Āropitamhi citake,",
  "tha-ap463:1.4_0": "sālapupphamapūjayiṁ.",
  "tha-ap463:2.1_0": "Satasahassito kappe,",
  "tha-ap463:2.2_0": "Yaṁ pupphamabhiropayiṁ;",
  "tha-ap463:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap463:2.4_0": "Citapūjāyidaṁ phalaṁ.",
  "tha-ap463:3.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap463:3.2_0": "bhavā sabbe samūhatā;",
  "tha-ap463:3.3_0": "Nāgova bandhanaṁ chetvā,",
  "tha-ap463:3.4_0": "viharāmi anāsavo.",
  "tha-ap463:4.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap463:4.2_0": "mama buddhassa santike;",
  "tha-ap463:4.3_0": "Tisso vijjā anuppattā,",
  "tha-ap463:4.4_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap463:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap463:5.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap463:5.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap463:5.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap463:6.1_0": "Itthaṁ sudaṁ āyasmā sālakusumiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap463:7.1_0": "Sālakusumiyattherassāpadānaṁ paṭhamaṁ."
}