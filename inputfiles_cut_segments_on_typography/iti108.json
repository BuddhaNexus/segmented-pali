{
  "iti108:0.1_0": "Itivuttaka 108",
  "iti108:0.2_0": "Catukkanipāta",
  "iti108:0.3_0": "Brāhmaṇadhammayāgavagga",
  "iti108:0.4_0": "Kuhasutta",
  "iti108:1.1_0": "Vuttañhetaṁ bhagavatā vuttamarahatāti me sutaṁ:",
  "iti108:2.1_0": "“Ye keci,",
  "iti108:2.1_1": "bhikkhave,",
  "iti108:2.1_2": "bhikkhū kuhā thaddhā lapā siṅgī unnaḷā asamāhitā, na me te,",
  "iti108:2.1_3": "bhikkhave,",
  "iti108:2.1_4": "bhikkhū māmakā.",
  "iti108:2.2_0": "Apagatā ca te,",
  "iti108:2.2_1": "bhikkhave,",
  "iti108:2.2_2": "bhikkhū imasmā dhammavinayā;",
  "iti108:2.2_3": "na ca te imasmiṁ dhammavinaye vuddhiṁ virūḷhiṁ vepullaṁ āpajjanti.",
  "iti108:2.3_0": "Ye ca kho,",
  "iti108:2.3_1": "bhikkhave,",
  "iti108:2.3_2": "bhikkhū nikkuhā nillapā dhīrā atthaddhā susamāhitā,",
  "iti108:2.3_3": "te kho me,",
  "iti108:2.3_4": "bhikkhave,",
  "iti108:2.3_5": "bhikkhū māmakā.",
  "iti108:2.4_0": "Anapagatā ca te,",
  "iti108:2.4_1": "bhikkhave,",
  "iti108:2.4_2": "bhikkhū imasmā dhammavinayā;",
  "iti108:2.4_3": "te ca imasmiṁ dhammavinaye vuddhiṁ virūḷhiṁ vepullaṁ āpajjantī”ti.",
  "iti108:2.5_0": "Etamatthaṁ bhagavā avoca.",
  "iti108:2.6_0": "Tatthetaṁ iti vuccati:",
  "iti108:3.1_0": "“Kuhā thaddhā lapā siṅgī,",
  "iti108:3.2_0": "unnaḷā asamāhitā;",
  "iti108:3.3_0": "Na te dhamme virūhanti,",
  "iti108:3.4_0": "sammāsambuddhadesite.",
  "iti108:4.1_0": "Nikkuhā nillapā dhīrā,",
  "iti108:4.2_0": "atthaddhā susamāhitā;",
  "iti108:4.3_0": "Te ve dhamme virūhanti,",
  "iti108:4.4_0": "sammāsambuddhadesite”ti.",
  "iti108:5.1_0": "Ayampi attho vutto bhagavatā, iti me sutanti.",
  "iti108:5.2_0": "Navamaṁ."
}