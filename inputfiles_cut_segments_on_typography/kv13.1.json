{
  "kv13.1:0.1_0": "Kathāvatthu",
  "kv13.1:0.2_0": "Anusayapaṇṇāsaka",
  "kv13.1:0.3_0": "Terasamavagga",
  "kv13.1:0.4_0": "Kappaṭṭhakathā",
  "kv13.1:1.1_0": "Kappaṭṭho kappaṁ tiṭṭheyyāti?",
  "kv13.1:1.2_0": "Āmantā.",
  "kv13.1:1.3_0": "Kappo ca saṇṭhāti buddho ca loke uppajjatīti?",
  "kv13.1:1.4_0": "Na hevaṁ vattabbe …pe….",
  "kv13.1:1.5_0": "Kappaṭṭho kappaṁ tiṭṭheyyāti?",
  "kv13.1:1.6_0": "Āmantā.",
  "kv13.1:1.7_0": "Kappo ca saṇṭhāti saṅgho ca bhijjatīti?",
  "kv13.1:1.8_0": "Na hevaṁ vattabbe …pe…",
  "kv13.1:1.9_0": "kappaṭṭho kappaṁ tiṭṭheyyāti?",
  "kv13.1:1.10_0": "Āmantā.",
  "kv13.1:1.11_0": "Kappo ca saṇṭhāti kappaṭṭho ca kappaṭṭhiyaṁ kammaṁ karotīti?",
  "kv13.1:1.12_0": "Na hevaṁ vattabbe …pe…",
  "kv13.1:1.13_0": "kappaṭṭho kappaṁ tiṭṭheyyāti?",
  "kv13.1:1.14_0": "Āmantā.",
  "kv13.1:1.15_0": "Kappo ca saṇṭhāti kappaṭṭho ca puggalo kālaṁ karotīti?",
  "kv13.1:1.16_0": "Na hevaṁ vattabbe …pe….",
  "kv13.1:2.1_0": "Kappaṭṭho kappaṁ tiṭṭheyyāti?",
  "kv13.1:2.2_0": "Āmantā.",
  "kv13.1:2.3_0": "Atītaṁ kappaṁ tiṭṭheyya, anāgataṁ kappaṁ tiṭṭheyyāti?",
  "kv13.1:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv13.1:2.5_0": "kappaṭṭho kappaṁ tiṭṭheyyāti?",
  "kv13.1:2.6_0": "Āmantā.",
  "kv13.1:2.7_0": "Dve kappe tiṭṭheyya … tayo kappe tiṭṭheyya … cattāro kappe tiṭṭheyyāti?",
  "kv13.1:2.8_0": "Na hevaṁ vattabbe …pe….",
  "kv13.1:3.1_0": "Kappaṭṭho kappaṁ tiṭṭheyyāti?",
  "kv13.1:3.2_0": "Āmantā.",
  "kv13.1:3.3_0": "Kappaṭṭho kappe ḍayhante kattha gacchatīti?",
  "kv13.1:3.4_0": "Aññaṁ lokadhātuṁ gacchatīti.",
  "kv13.1:3.5_0": "Mato gacchati, vehāsaṁ gacchatīti?",
  "kv13.1:3.6_0": "Mato gacchatīti.",
  "kv13.1:3.7_0": "Kappaṭṭhiyaṁ kammaṁ aparāpariyavepakkanti?",
  "kv13.1:3.8_0": "Na hevaṁ vattabbe …pe…",
  "kv13.1:3.9_0": "vehāsaṁ gacchatīti?",
  "kv13.1:3.10_0": "Āmantā.",
  "kv13.1:3.11_0": "Kappaṭṭho iddhimāti?",
  "kv13.1:3.12_0": "Na hevaṁ vattabbe …pe…",
  "kv13.1:3.13_0": "kappaṭṭho iddhimāti?",
  "kv13.1:3.14_0": "Āmantā.",
  "kv13.1:3.15_0": "Kappaṭṭhena chandiddhipādo bhāvito vīriyiddhipādo bhāvito cittiddhipādo bhāvito vīmaṁsiddhipādo bhāvitoti?",
  "kv13.1:3.16_0": "Na hevaṁ vattabbe …pe….",
  "kv13.1:4.1_0": "Na vattabbaṁ—",
  "kv13.1:4.2_0": "“kappaṭṭho kappaṁ tiṭṭheyyā”ti?",
  "kv13.1:4.3_0": "Āmantā.",
  "kv13.1:4.4_0": "Nanu vuttaṁ bhagavatā—",
  "kv13.1:5.1_0": "“Āpāyiko nerayiko,",
  "kv13.1:5.2_0": "Kappaṭṭho saṅghabhedako;",
  "kv13.1:5.3_0": "Vaggarato adhammaṭṭho,",
  "kv13.1:5.4_0": "Yogakkhemā padhaṁsati;",
  "kv13.1:5.5_0": "Saṅghaṁ samaggaṁ bhetvāna,",
  "kv13.1:5.6_0": "Kappaṁ nirayamhi paccatī”ti.",
  "kv13.1:6.1_0": "Attheva suttantoti?",
  "kv13.1:6.2_0": "Āmantā.",
  "kv13.1:6.3_0": "Tena hi kappaṭṭho kappaṁ tiṭṭheyyāti.",
  "kv13.1:6.4_0": "Kappaṭṭhakathā niṭṭhitā."
}