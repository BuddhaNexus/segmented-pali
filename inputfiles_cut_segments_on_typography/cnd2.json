{
  "cnd2:0.1_0": "Cūḷaniddesa",
  "cnd2:0.2_0": "Pārāyanavaggagāthā",
  "cnd2:0.3_0": "Pucchā",
  "cnd2:1.0.1_0": "1. Ajitamāṇavapucchā",
  "cnd2:1.1_0": "“Kenassu nivuto loko,",
  "cnd2:1.2_0": "(iccāyasmā ajito)",
  "cnd2:1.3_0": "Kenassu nappakāsati;",
  "cnd2:1.4_0": "Kissābhilepanaṁ brūsi,",
  "cnd2:1.5_0": "Kiṁsu tassa mahabbhayaṁ”.",
  "cnd2:2.1_0": "“Avijjāya nivuto loko,",
  "cnd2:2.2_0": "(ajitāti bhagavā)",
  "cnd2:2.3_0": "Vevicchā pamādā nappakāsati;",
  "cnd2:2.4_0": "Jappābhilepanaṁ brūmi,",
  "cnd2:2.5_0": "Dukkhamassa mahabbhayaṁ”.",
  "cnd2:3.1_0": "“Savanti sabbadhi sotā,",
  "cnd2:3.2_0": "(iccāyasmā ajito)",
  "cnd2:3.3_0": "Sotānaṁ kiṁ nivāraṇaṁ;",
  "cnd2:3.4_0": "Sotānaṁ saṁvaraṁ brūhi,",
  "cnd2:3.5_0": "Kena sotā pidhiyyare”.",
  "cnd2:4.1_0": "“Yāni sotāni lokasmiṁ,",
  "cnd2:4.2_0": "(ajitāti bhagavā)",
  "cnd2:4.3_0": "Sati tesaṁ nivāraṇaṁ;",
  "cnd2:4.4_0": "Sotānaṁ saṁvaraṁ brūmi,",
  "cnd2:4.5_0": "Paññāyete pidhiyyare”.",
  "cnd2:5.1_0": "“Paññā ceva sati cāpi,",
  "cnd2:5.2_0": "(iccāyasmā ajito)",
  "cnd2:5.3_0": "Nāmarūpañca mārisa;",
  "cnd2:5.4_0": "Etaṁ me puṭṭho pabrūhi,",
  "cnd2:5.5_0": "Katthetaṁ uparujjhati”.",
  "cnd2:6.1_0": "“Yametaṁ pañhaṁ apucchi,",
  "cnd2:6.2_0": "ajita taṁ vadāmi te;",
  "cnd2:6.3_0": "Yattha nāmañca rūpañca,",
  "cnd2:6.4_0": "asesaṁ uparujjhati;",
  "cnd2:6.5_0": "Viññāṇassa nirodhena,",
  "cnd2:6.6_0": "etthetaṁ uparujjhati”.",
  "cnd2:7.1_0": "“Ye ca saṅkhātadhammāse,",
  "cnd2:7.2_0": "ye ca sekhā puthū idha;",
  "cnd2:7.3_0": "Tesaṁ me nipako iriyaṁ,",
  "cnd2:7.4_0": "puṭṭho pabrūhi mārisa”.",
  "cnd2:8.1_0": "“Kāmesu nābhigijjheyya,",
  "cnd2:8.2_0": "Manasānāvilo siyā;",
  "cnd2:8.3_0": "Kusalo sabbadhammānaṁ,",
  "cnd2:8.4_0": "Sato bhikkhu paribbaje”ti.",
  "cnd2:8.5_0": "Ajitamāṇavapucchā paṭhamā.",
  "cnd2:9.0_0": "2. Tissametteyyamāṇavapucchā",
  "cnd2:9.1_0": "“Kodha santusito loke,",
  "cnd2:9.2_0": "(iccāyasmā tissametteyyo)",
  "cnd2:9.3_0": "Kassa no santi iñjitā;",
  "cnd2:9.4_0": "Ko ubhantamabhiññāya,",
  "cnd2:9.5_0": "Majjhe mantā na lippati;",
  "cnd2:9.6_0": "Kaṁ brūsi mahāpurisoti,",
  "cnd2:9.7_0": "Ko idha sibbinimaccagā”.",
  "cnd2:10.1_0": "“Kāmesu brahmacariyavā,",
  "cnd2:10.2_0": "(metteyyāti bhagavā)",
  "cnd2:10.3_0": "Vītataṇho sadā sato;",
  "cnd2:10.4_0": "Saṅkhāya nibbuto bhikkhu,",
  "cnd2:10.5_0": "Tassa no santi iñjitā.",
  "cnd2:11.1_0": "So ubhantamabhiññāya,",
  "cnd2:11.2_0": "Majjhe mantā na lippati;",
  "cnd2:11.3_0": "Taṁ brūmi mahāpurisoti,",
  "cnd2:11.4_0": "So idha sibbinimaccagā”ti.",
  "cnd2:11.5_0": "Tissametteyyamāṇavapucchā dutiyā.",
  "cnd2:12.0_0": "3. Puṇṇakamāṇavapucchā",
  "cnd2:12.1_0": "“Anejaṁ mūladassāviṁ,",
  "cnd2:12.2_0": "(iccāyasmā puṇṇako)",
  "cnd2:12.3_0": "Atthi pañhena āgamaṁ;",
  "cnd2:12.4_0": "Kiṁ nissitā isayo manujā,",
  "cnd2:12.5_0": "Khattiyā brāhmaṇā devatānaṁ;",
  "cnd2:12.6_0": "Yaññamakappayiṁsu puthūdha loke,",
  "cnd2:12.7_0": "Pucchāmi taṁ bhagavā brūhi metaṁ”.",
  "cnd2:13.1_0": "“Ye kecime isayo manujā,",
  "cnd2:13.2_0": "(puṇṇakāti bhagavā)",
  "cnd2:13.3_0": "Khattiyā brāhmaṇā devatānaṁ;",
  "cnd2:13.4_0": "Yaññamakappayiṁsu puthūdha loke,",
  "cnd2:13.5_0": "Āsīsamānā puṇṇaka itthattaṁ;",
  "cnd2:13.6_0": "Jaraṁ sitā yaññamakappayiṁsu”.",
  "cnd2:14.1_0": "“Ye kecime isayo manujā,",
  "cnd2:14.2_0": "(iccāyasmā puṇṇako)",
  "cnd2:14.3_0": "Khattiyā brāhmaṇā devatānaṁ;",
  "cnd2:14.4_0": "Yaññamakappayiṁsu puthūdha loke,",
  "cnd2:14.5_0": "Kaccisu te bhagavā yaññapathe appamattā;",
  "cnd2:14.6_0": "Atāruṁ jātiñca jarañca mārisa,",
  "cnd2:14.7_0": "Pucchāmi taṁ bhagavā brūhi metaṁ”.",
  "cnd2:15.1_0": "“Āsīsanti thomayanti abhijappanti juhanti,",
  "cnd2:15.2_0": "(puṇṇakāti bhagavā)",
  "cnd2:15.3_0": "Kāmābhijappanti paṭicca lābhaṁ;",
  "cnd2:15.4_0": "Te yājayogā bhavarāgarattā,",
  "cnd2:15.5_0": "Nātariṁsu jātijaranti brūmi”.",
  "cnd2:16.1_0": "“Te ce nātariṁsu yājayogā,",
  "cnd2:16.2_0": "(iccāyasmā puṇṇako)",
  "cnd2:16.3_0": "Yaññehi jātiñca jarañca mārisa;",
  "cnd2:16.4_0": "Atha ko carahi devamanussaloke,",
  "cnd2:16.5_0": "Atāri jātiñca jarañca mārisa;",
  "cnd2:16.6_0": "Pucchāmi taṁ bhagavā brūhi metaṁ”.",
  "cnd2:17.1_0": "“Saṅkhāya lokasmi paroparāni,",
  "cnd2:17.2_0": "(puṇṇakāti bhagavā)",
  "cnd2:17.3_0": "Yassiñjitaṁ natthi kuhiñci loke;",
  "cnd2:17.4_0": "Santo vidhūmo anīgho nirāso,",
  "cnd2:17.5_0": "Atāri so jātijaranti brūmī”ti.",
  "cnd2:17.6_0": "Puṇṇakamāṇavapucchā tatiyā.",
  "cnd2:18.0_0": "4. Mettagūmāṇavapucchā",
  "cnd2:18.1_0": "“Pucchāmi taṁ bhagavā brūhi metaṁ,",
  "cnd2:18.2_0": "(iccāyasmā mettagū)",
  "cnd2:18.3_0": "Maññāmi taṁ vedaguṁ bhāvitattaṁ;",
  "cnd2:18.4_0": "Kuto nu dukkhā samudāgatā ime,",
  "cnd2:18.5_0": "Ye keci lokasmimanekarūpā”.",
  "cnd2:19.1_0": "“Dukkhassa ve maṁ pabhavaṁ apucchasi,",
  "cnd2:19.2_0": "(mettagūti bhagavā)",
  "cnd2:19.3_0": "Taṁ te pavakkhāmi yathā pajānaṁ;",
  "cnd2:19.4_0": "Upadhinidānā pabhavanti dukkhā,",
  "cnd2:19.5_0": "Ye keci lokasmimanekarūpā.",
  "cnd2:20.1_0": "Yo ve avidvā upadhiṁ karoti,",
  "cnd2:20.2_0": "Punappunaṁ dukkhamupeti mando;",
  "cnd2:20.3_0": "Tasmā pajānaṁ upadhiṁ na kayirā,",
  "cnd2:20.4_0": "Dukkhassa jātippabhavānupassī”.",
  "cnd2:21.1_0": "“Yaṁ taṁ apucchimha akittayī no,",
  "cnd2:21.2_0": "Aññaṁ taṁ pucchāma tadiṅgha brūhi;",
  "cnd2:21.3_0": "‘Kathaṁ nu dhīrā vitaranti oghaṁ,",
  "cnd2:21.4_0": "Jātiṁ jaraṁ sokapariddavañca’;",
  "cnd2:21.5_0": "Taṁ me muni sādhu viyākarohi,",
  "cnd2:21.6_0": "Tathā hi te vidito esa dhammo”.",
  "cnd2:22.1_0": "“Kittayissāmi te dhammaṁ,",
  "cnd2:22.2_0": "(mettagūti bhagavā)",
  "cnd2:22.3_0": "Diṭṭhe dhamme anītihaṁ;",
  "cnd2:22.4_0": "Yaṁ viditvā sato caraṁ,",
  "cnd2:22.5_0": "Tare loke visattikaṁ”.",
  "cnd2:23.1_0": "“Tañcāhaṁ abhinandāmi,",
  "cnd2:23.2_0": "mahesi dhammamuttamaṁ;",
  "cnd2:23.3_0": "Yaṁ viditvā sato caraṁ,",
  "cnd2:23.4_0": "tare loke visattikaṁ”.",
  "cnd2:24.1_0": "“Yaṁ kiñci sampajānāsi,",
  "cnd2:24.2_0": "(mettagūti bhagavā)",
  "cnd2:24.3_0": "Uddhaṁ adho tiriyañcāpi majjhe;",
  "cnd2:24.4_0": "Etesu nandiñca nivesanañca,",
  "cnd2:24.5_0": "Panujja viññāṇaṁ bhave na tiṭṭhe.",
  "cnd2:25.1_0": "Evaṁvihārī sato appamatto,",
  "cnd2:25.2_0": "Bhikkhu caraṁ hitvā mamāyitāni;",
  "cnd2:25.3_0": "Jātiṁ jaraṁ sokapariddavañca,",
  "cnd2:25.4_0": "Idheva vidvā pajaheyya dukkhaṁ”.",
  "cnd2:26.1_0": "“Etābhinandāmi vaco mahesino,",
  "cnd2:26.2_0": "Sukittitaṁ gotamanūpadhīkaṁ;",
  "cnd2:26.3_0": "Addhā hi bhagavā pahāsi dukkhaṁ,",
  "cnd2:26.4_0": "Tathā hi te vidito esa dhammo.",
  "cnd2:27.1_0": "Te cāpi nūnappajaheyyu dukkhaṁ,",
  "cnd2:27.2_0": "Ye tvaṁ muni aṭṭhitaṁ ovadeyya;",
  "cnd2:27.3_0": "Taṁ taṁ namassāmi samecca nāga,",
  "cnd2:27.4_0": "Appeva maṁ bhagavā aṭṭhitaṁ ovadeyya”.",
  "cnd2:28.1_0": "“Yaṁ brāhmaṇaṁ vedagumābhijaññā,",
  "cnd2:28.2_0": "Akiñcanaṁ kāmabhave asattaṁ;",
  "cnd2:28.3_0": "Addhā hi so oghamimaṁ atāri,",
  "cnd2:28.4_0": "Tiṇṇo ca pāraṁ akhilo akaṅkho.",
  "cnd2:29.1_0": "Vidvā ca yo vedagū naro idha,",
  "cnd2:29.2_0": "Bhavābhave saṅgamimaṁ visajja;",
  "cnd2:29.3_0": "So vītataṇho anīgho nirāso,",
  "cnd2:29.4_0": "Atāri so jātijaranti brūmī”ti.",
  "cnd2:29.5_0": "Mettagūmāṇavapucchā catutthī.",
  "cnd2:30.0_0": "5. Dhotakamāṇavapucchā",
  "cnd2:30.1_0": "“Pucchāmi taṁ bhagavā brūhi metaṁ,",
  "cnd2:30.2_0": "(iccāyasmā dhotako)",
  "cnd2:30.3_0": "Vācābhikaṅkhāmi mahesi tuyhaṁ;",
  "cnd2:30.4_0": "Tava sutvāna nigghosaṁ,",
  "cnd2:30.5_0": "Sikkhe nibbānamattano”.",
  "cnd2:31.1_0": "“Tenahātappaṁ karohi,",
  "cnd2:31.2_0": "(dhotakāti bhagavā)",
  "cnd2:31.3_0": "Idheva nipako sato;",
  "cnd2:31.4_0": "Ito sutvāna nigghosaṁ,",
  "cnd2:31.5_0": "Sikkhe nibbānamattano”.",
  "cnd2:32.1_0": "“Passāmahaṁ devamanussaloke,",
  "cnd2:32.2_0": "Akiñcanaṁ brāhmaṇamiriyamānaṁ;",
  "cnd2:32.3_0": "Taṁ taṁ namassāmi samantacakkhu,",
  "cnd2:32.4_0": "Pamuñca maṁ sakka kathaṅkathāhi”.",
  "cnd2:33.1_0": "“Nāhaṁ sahissāmi pamocanāya,",
  "cnd2:33.2_0": "Kathaṅkathiṁ dhotaka kañci loke;",
  "cnd2:33.3_0": "Dhammañca seṭṭhaṁ abhijānamāno,",
  "cnd2:33.4_0": "Evaṁ tuvaṁ oghamimaṁ taresi”.",
  "cnd2:34.1_0": "“Anusāsa brahme karuṇāyamāno,",
  "cnd2:34.2_0": "Vivekadhammaṁ yamahaṁ vijaññaṁ;",
  "cnd2:34.3_0": "Yathāhaṁ ākāsova abyāpajjamāno,",
  "cnd2:34.4_0": "Idheva santo asito careyyaṁ”.",
  "cnd2:35.1_0": "“Kittayissāmi te santiṁ,",
  "cnd2:35.2_0": "(dhotakāti bhagavā)",
  "cnd2:35.3_0": "Diṭṭhe dhamme anītihaṁ;",
  "cnd2:35.4_0": "Yaṁ viditvā sato caraṁ,",
  "cnd2:35.5_0": "Tare loke visattikaṁ”.",
  "cnd2:36.1_0": "“Tañcāhaṁ abhinandāmi,",
  "cnd2:36.2_0": "mahesi santimuttamaṁ;",
  "cnd2:36.3_0": "Yaṁ viditvā sato caraṁ,",
  "cnd2:36.4_0": "tare loke visattikaṁ”.",
  "cnd2:37.1_0": "“Yaṁ kiñci sampajānāsi,",
  "cnd2:37.2_0": "(dhotakāti bhagavā)",
  "cnd2:37.3_0": "Uddhaṁ adho tiriyañcāpi majjhe;",
  "cnd2:37.4_0": "Etaṁ viditvā saṅgoti loke,",
  "cnd2:37.5_0": "Bhavābhavāya mākāsi taṇhan”ti.",
  "cnd2:37.6_0": "Dhotakamāṇavapucchā pañcamī.",
  "cnd2:38.0_0": "6. Upasīvamāṇavapucchā",
  "cnd2:38.1_0": "“Eko ahaṁ sakka mahantamoghaṁ,",
  "cnd2:38.2_0": "(iccāyasmā upasīvo)",
  "cnd2:38.3_0": "Anissito no visahāmi tārituṁ;",
  "cnd2:38.4_0": "Ārammaṇaṁ brūhi samantacakkhu,",
  "cnd2:38.5_0": "Yaṁ nissito oghamimaṁ tareyyaṁ”.",
  "cnd2:39.1_0": "“Ākiñcaññaṁ pekkhamāno satimā,",
  "cnd2:39.2_0": "(upasīvāti bhagavā)",
  "cnd2:39.3_0": "Natthīti nissāya tarassu oghaṁ;",
  "cnd2:39.4_0": "Kāme pahāya virato kathāhi,",
  "cnd2:39.5_0": "Taṇhakkhayaṁ nattamahābhipassa”.",
  "cnd2:40.1_0": "“Sabbesu kāmesu yo vītarāgo,",
  "cnd2:40.2_0": "(iccāyasmā upasīvo)",
  "cnd2:40.3_0": "Ākiñcaññaṁ nissito hitvā maññaṁ;",
  "cnd2:40.4_0": "Saññāvimokkhe paramedhimutto,",
  "cnd2:40.5_0": "Tiṭṭhe nu so tattha anānuyāyī”.",
  "cnd2:41.1_0": "“Sabbesu kāmesu yo vītarāgo,",
  "cnd2:41.2_0": "(upasīvāti bhagavā)",
  "cnd2:41.3_0": "Ākiñcaññaṁ nissito hitvā maññaṁ;",
  "cnd2:41.4_0": "Saññāvimokkhe paramedhimutto,",
  "cnd2:41.5_0": "Tiṭṭheyya so tattha anānuyāyī”.",
  "cnd2:42.1_0": "“Tiṭṭhe ce so tattha anānuyāyī,",
  "cnd2:42.2_0": "Pūgampi vassānaṁ samantacakkhu;",
  "cnd2:42.3_0": "Tattheva so sītisiyā vimutto,",
  "cnd2:42.4_0": "Cavetha viññāṇaṁ tathāvidhassa”.",
  "cnd2:43.1_0": "“Acci yathā vātavegena khittā,",
  "cnd2:43.2_0": "(upasīvāti bhagavā)",
  "cnd2:43.3_0": "Atthaṁ paleti na upeti saṅkhaṁ;",
  "cnd2:43.4_0": "Evaṁ munī nāmakāyā vimutto,",
  "cnd2:43.5_0": "Atthaṁ paleti na upeti saṅkhaṁ”.",
  "cnd2:44.1_0": "“Atthaṅgato so uda vā so natthi,",
  "cnd2:44.2_0": "Udāhu ve sassatiyā arogo;",
  "cnd2:44.3_0": "Taṁ me munī sādhu viyākarohi,",
  "cnd2:44.4_0": "Tathā hi te vidito esa dhammo”.",
  "cnd2:45.1_0": "“Atthaṅgatassa na pamāṇamatthi,",
  "cnd2:45.2_0": "(upasīvāti bhagavā)",
  "cnd2:45.3_0": "Yena naṁ vajjuṁ taṁ tassa natthi;",
  "cnd2:45.4_0": "Sabbesu dhammesu samūhatesu,",
  "cnd2:45.5_0": "Samūhatā vādapathāpi sabbe”ti.",
  "cnd2:45.6_0": "Upasīvamāṇavapucchā chaṭṭhī.",
  "cnd2:46.0_0": "7. Nandamāṇavapucchā",
  "cnd2:46.1_0": "“Santi loke munayo,",
  "cnd2:46.2_0": "(iccāyasmā nando)",
  "cnd2:46.3_0": "Janā vadanti tayidaṁ kathaṁsu;",
  "cnd2:46.4_0": "Ñāṇūpapannaṁ muni no vadanti,",
  "cnd2:46.5_0": "Udāhu ve jīvitenūpapannaṁ”.",
  "cnd2:47.1_0": "“Na diṭṭhiyā na sutiyā na ñāṇena,",
  "cnd2:47.2_0": "Munīdha nanda kusalā vadanti;",
  "cnd2:47.3_0": "Visenikatvā anīghā nirāsā,",
  "cnd2:47.4_0": "Caranti ye te munayoti brūmi”.",
  "cnd2:48.1_0": "“Ye kecime samaṇabrāhmaṇāse,",
  "cnd2:48.2_0": "(iccāyasmā nando)",
  "cnd2:48.3_0": "Diṭṭhassutenāpi vadanti suddhiṁ;",
  "cnd2:48.4_0": "Sīlabbatenāpi vadanti suddhiṁ,",
  "cnd2:48.5_0": "Anekarūpena vadanti suddhiṁ;",
  "cnd2:48.6_0": "Kaccissu te bhagavā tattha yatā carantā,",
  "cnd2:48.7_0": "Atāru jātiñca jarañca mārisa;",
  "cnd2:48.8_0": "Pucchāmi taṁ bhagavā brūhi metaṁ”.",
  "cnd2:49.1_0": "“Ye kecime samaṇabrāhmaṇāse,",
  "cnd2:49.2_0": "(nandāti bhagavā)",
  "cnd2:49.3_0": "Diṭṭhassutenāpi vadanti suddhiṁ;",
  "cnd2:49.4_0": "Sīlabbatenāpi vadanti suddhiṁ,",
  "cnd2:49.5_0": "Anekarūpena vadanti suddhiṁ;",
  "cnd2:49.6_0": "Kiñcāpi te tattha yatā caranti,",
  "cnd2:49.7_0": "Nātariṁsu jātijaranti brūmi”.",
  "cnd2:50.1_0": "“Ye kecime samaṇabrāhmaṇāse,",
  "cnd2:50.2_0": "(iccāyasmā nando)",
  "cnd2:50.3_0": "Diṭṭhassutenāpi vadanti suddhiṁ;",
  "cnd2:50.4_0": "Sīlabbatenāpi vadanti suddhiṁ,",
  "cnd2:50.5_0": "Anekarūpena vadanti suddhiṁ;",
  "cnd2:50.6_0": "Te ce muni brūsi anoghatiṇṇe,",
  "cnd2:50.7_0": "Atha ko carahi devamanussaloke;",
  "cnd2:50.8_0": "Atāri jātiñca jarañca mārisa,",
  "cnd2:50.9_0": "Pucchāmi taṁ bhagavā brūhi metaṁ”.",
  "cnd2:51.1_0": "“Nāhaṁ sabbe samaṇabrāhmaṇāse,",
  "cnd2:51.2_0": "(nandāti bhagavā)",
  "cnd2:51.3_0": "Jātijarāya nivutāti brūmi;",
  "cnd2:51.4_0": "Ye sīdha diṭṭhaṁ va sutaṁ mutaṁ vā,",
  "cnd2:51.5_0": "Sīlabbataṁ vāpi pahāya sabbaṁ;",
  "cnd2:51.6_0": "Anekarūpampi pahāya sabbaṁ,",
  "cnd2:51.7_0": "Taṇhaṁ pariññāya anāsavāse;",
  "cnd2:51.8_0": "Te ve narā oghatiṇṇāti brūmi”.",
  "cnd2:52.1_0": "“Etābhinandāmi vaco mahesino,",
  "cnd2:52.2_0": "Sukittitaṁ gotamanūpadhīkaṁ;",
  "cnd2:52.3_0": "Ye sīdha diṭṭhaṁ va sutaṁ mutaṁ vā,",
  "cnd2:52.4_0": "Sīlabbataṁ vāpi pahāya sabbaṁ;",
  "cnd2:52.5_0": "Anekarūpampi pahāya sabbaṁ,",
  "cnd2:52.6_0": "Taṇhaṁ pariññāya anāsavāse;",
  "cnd2:52.7_0": "Ahampi te oghatiṇṇāti brūmī”ti.",
  "cnd2:52.8_0": "Nandamāṇavapucchā sattamā.",
  "cnd2:53.0_0": "8. Hemakamāṇavapucchā",
  "cnd2:53.1_0": "“Ye me pubbe viyākaṁsu,",
  "cnd2:53.2_0": "(iccāyasmā hemako)",
  "cnd2:53.3_0": "Huraṁ gotamasāsanā;",
  "cnd2:53.4_0": "Iccāsi iti bhavissati,",
  "cnd2:53.5_0": "Sabbaṁ taṁ itihītihaṁ;",
  "cnd2:53.6_0": "Sabbaṁ taṁ takkavaḍḍhanaṁ,",
  "cnd2:53.7_0": "Nāhaṁ tattha abhiramiṁ.",
  "cnd2:54.1_0": "Tvañca me dhammamakkhāhi,",
  "cnd2:54.2_0": "taṇhānigghātanaṁ muni;",
  "cnd2:54.3_0": "Yaṁ viditvā sato caraṁ,",
  "cnd2:54.4_0": "tare loke visattikaṁ”.",
  "cnd2:55.1_0": "“Idha diṭṭhasutamutaviññātesu,",
  "cnd2:55.2_0": "Piyarūpesu hemaka;",
  "cnd2:55.3_0": "Chandarāgavinodanaṁ,",
  "cnd2:55.4_0": "Nibbānapadamaccutaṁ.",
  "cnd2:56.1_0": "Etadaññāya ye satā,",
  "cnd2:56.2_0": "Diṭṭhadhammābhinibbutā;",
  "cnd2:56.3_0": "Upasantā ca te sadā,",
  "cnd2:56.4_0": "Tiṇṇā loke visattikan”ti.",
  "cnd2:56.5_0": "Hemakamāṇavapucchā aṭṭhamā.",
  "cnd2:57.0_0": "9. Todeyyamāṇavapucchā",
  "cnd2:57.1_0": "“Yasmiṁ kāmā na vasanti,",
  "cnd2:57.2_0": "(iccāyasmā todeyyo)",
  "cnd2:57.3_0": "Taṇhā yassa na vijjati;",
  "cnd2:57.4_0": "Kathaṅkathā ca yo tiṇṇo,",
  "cnd2:57.5_0": "Vimokkho tassa kīdiso”.",
  "cnd2:58.1_0": "“Yasmiṁ kāmā na vasanti,",
  "cnd2:58.2_0": "(todeyyāti bhagavā)",
  "cnd2:58.3_0": "Taṇhā yassa na vijjati;",
  "cnd2:58.4_0": "Kathaṅkathā ca yo tiṇṇo,",
  "cnd2:58.5_0": "Vimokkho tassa nāparo”.",
  "cnd2:59.1_0": "“Nirāsaso so uda āsasāno,",
  "cnd2:59.2_0": "Paññāṇavā so uda paññakappī;",
  "cnd2:59.3_0": "Muniṁ ahaṁ sakka yathā vijaññaṁ,",
  "cnd2:59.4_0": "Taṁ me viyācikkha samantacakkhu”.",
  "cnd2:60.1_0": "“Nirāsaso so na ca āsasāno,",
  "cnd2:60.2_0": "Paññāṇavā so na ca paññakappī;",
  "cnd2:60.3_0": "Evampi todeyya muniṁ vijāna,",
  "cnd2:60.4_0": "Akiñcanaṁ kāmabhave asattan”ti.",
  "cnd2:60.5_0": "Todeyyamāṇavapucchā navamā.",
  "cnd2:61.0_0": "10. Kappamāṇavapucchā",
  "cnd2:61.1_0": "“Majjhe sarasmiṁ tiṭṭhataṁ,",
  "cnd2:61.2_0": "(iccāyasmā kappo)",
  "cnd2:61.3_0": "Oghe jāte mahabbhaye;",
  "cnd2:61.4_0": "Jarāmaccuparetānaṁ,",
  "cnd2:61.5_0": "Dīpaṁ pabrūhi mārisa;",
  "cnd2:61.6_0": "Tvañca me dīpamakkhāhi,",
  "cnd2:61.7_0": "Yathāyidaṁ nāparaṁ siyā”.",
  "cnd2:62.1_0": "“Majjhe sarasmiṁ tiṭṭhataṁ,",
  "cnd2:62.2_0": "(kappāti bhagavā)",
  "cnd2:62.3_0": "Oghe jāte mahabbhaye;",
  "cnd2:62.4_0": "Jarāmaccuparetānaṁ,",
  "cnd2:62.5_0": "Dīpaṁ pabrūmi kappa te.",
  "cnd2:63.1_0": "Akiñcanaṁ anādānaṁ,",
  "cnd2:63.2_0": "etaṁ dīpaṁ anāparaṁ;",
  "cnd2:63.3_0": "Nibbānaṁ iti naṁ brūmi,",
  "cnd2:63.4_0": "jarāmaccuparikkhayaṁ.",
  "cnd2:64.1_0": "Etadaññāya ye satā,",
  "cnd2:64.2_0": "Diṭṭhadhammābhinibbutā;",
  "cnd2:64.3_0": "Na te māravasānugā,",
  "cnd2:64.4_0": "Na te mārassa paddhagū”ti.",
  "cnd2:64.5_0": "Kappamāṇavapucchā dasamā.",
  "cnd2:65.0_0": "11. Jatukaṇṇimāṇavapucchā",
  "cnd2:65.1_0": "“Sutvānahaṁ vīramakāmakāmiṁ,",
  "cnd2:65.2_0": "(iccāyasmā jatukaṇṇi)",
  "cnd2:65.3_0": "Oghātigaṁ puṭṭhumakāmamāgamaṁ;",
  "cnd2:65.4_0": "Santipadaṁ brūhi sahajanetta,",
  "cnd2:65.5_0": "Yathātacchaṁ bhagavā brūhi metaṁ.",
  "cnd2:66.1_0": "Bhagavā hi kāme abhibhuyya iriyati,",
  "cnd2:66.2_0": "Ādiccova pathaviṁ tejī tejasā;",
  "cnd2:66.3_0": "Parittapaññassa me bhūripañña,",
  "cnd2:66.4_0": "Ācikkha dhammaṁ yamahaṁ vijaññaṁ;",
  "cnd2:66.5_0": "Jātijarāya idha vippahānaṁ”.",
  "cnd2:67.1_0": "“Kāmesu vinaya gedhaṁ,",
  "cnd2:67.2_0": "(jatukaṇṇīti bhagavā)",
  "cnd2:67.3_0": "Nekkhammaṁ daṭṭhu khemato;",
  "cnd2:67.4_0": "Uggahitaṁ nirattaṁ vā,",
  "cnd2:67.5_0": "Mā te vijjittha kiñcanaṁ.",
  "cnd2:68.1_0": "Yaṁ pubbe taṁ visosehi,",
  "cnd2:68.2_0": "pacchā te māhu kiñcanaṁ;",
  "cnd2:68.3_0": "Majjhe ce no gahessasi,",
  "cnd2:68.4_0": "upasanto carissasi.",
  "cnd2:69.1_0": "Sabbaso nāmarūpasmiṁ,",
  "cnd2:69.2_0": "Vītagedhassa brāhmaṇa;",
  "cnd2:69.3_0": "Āsavāssa na vijjanti,",
  "cnd2:69.4_0": "Yehi maccuvasaṁ vaje”ti.",
  "cnd2:69.5_0": "Jatukaṇṇimāṇavapucchā ekādasamā.",
  "cnd2:70.0_0": "12. Bhadrāvudhamāṇavapucchā",
  "cnd2:70.1_0": "“Okañjahaṁ taṇhacchidaṁ anejaṁ,",
  "cnd2:70.2_0": "(iccāyasmā bhadrāvudho)",
  "cnd2:70.3_0": "Nandiñjahaṁ oghatiṇṇaṁ vimuttaṁ;",
  "cnd2:70.4_0": "Kappañjahaṁ abhiyāce sumedhaṁ,",
  "cnd2:70.5_0": "Sutvāna nāgassa apanamissanti ito.",
  "cnd2:71.1_0": "Nānājanā janapadehi saṅgatā,",
  "cnd2:71.2_0": "Tava vīra vākyaṁ abhikaṅkhamānā;",
  "cnd2:71.3_0": "Tesaṁ tuvaṁ sādhu viyākarohi,",
  "cnd2:71.4_0": "Tathā hi te vidito esa dhammo”.",
  "cnd2:72.1_0": "“Ādānataṇhaṁ vinayetha sabbaṁ,",
  "cnd2:72.2_0": "(bhadrāvudhāti bhagavā)",
  "cnd2:72.3_0": "Uddhaṁ adho tiriyañcāpi majjhe;",
  "cnd2:72.4_0": "Yaṁ yañhi lokasmimupādiyanti,",
  "cnd2:72.5_0": "Teneva māro anveti jantuṁ.",
  "cnd2:73.1_0": "Tasmā pajānaṁ na upādiyetha,",
  "cnd2:73.2_0": "Bhikkhu sato kiñcanaṁ sabbaloke;",
  "cnd2:73.3_0": "Ādānasatte iti pekkhamāno,",
  "cnd2:73.4_0": "Pajaṁ imaṁ maccudheyye visattan”ti.",
  "cnd2:73.5_0": "Bhadrāvudhamāṇavapucchā dvādasamā.",
  "cnd2:74.0_0": "13. Udayamāṇavapucchā",
  "cnd2:74.1_0": "“Jhāyiṁ virajamāsīnaṁ,",
  "cnd2:74.2_0": "(iccāyasmā udayo)",
  "cnd2:74.3_0": "Katakiccaṁ anāsavaṁ;",
  "cnd2:74.4_0": "Pāraguṁ sabbadhammānaṁ,",
  "cnd2:74.5_0": "Atthi pañhena āgamaṁ;",
  "cnd2:74.6_0": "Aññāvimokkhaṁ pabrūhi,",
  "cnd2:74.7_0": "Avijjāya pabhedanaṁ”.",
  "cnd2:75.1_0": "“Pahānaṁ kāmacchandānaṁ,",
  "cnd2:75.2_0": "(udayāti bhagavā)",
  "cnd2:75.3_0": "Domanassāna cūbhayaṁ;",
  "cnd2:75.4_0": "Thinassa ca panūdanaṁ,",
  "cnd2:75.5_0": "Kukkuccānaṁ nivāraṇaṁ.",
  "cnd2:76.1_0": "Upekkhāsatisaṁsuddhaṁ,",
  "cnd2:76.2_0": "dhammatakkapurejavaṁ;",
  "cnd2:76.3_0": "Aññāvimokkhaṁ pabrūmi,",
  "cnd2:76.4_0": "avijjāya pabhedanaṁ”.",
  "cnd2:77.1_0": "“Kiṁsu saṁyojano loko,",
  "cnd2:77.2_0": "kiṁsu tassa vicāraṇaṁ;",
  "cnd2:77.3_0": "Kissassa vippahānena,",
  "cnd2:77.4_0": "nibbānaṁ iti vuccati”.",
  "cnd2:78.1_0": "“Nandisaṁyojano loko,",
  "cnd2:78.2_0": "vitakkassa vicāraṇaṁ;",
  "cnd2:78.3_0": "Taṇhāya vippahānena,",
  "cnd2:78.4_0": "nibbānaṁ iti vuccati”.",
  "cnd2:79.1_0": "“Kathaṁ satassa carato,",
  "cnd2:79.2_0": "viññāṇaṁ uparujjhati;",
  "cnd2:79.3_0": "Bhagavantaṁ puṭṭhumāgamma,",
  "cnd2:79.4_0": "taṁ suṇoma vaco tava”.",
  "cnd2:80.1_0": "“Ajjhattañca bahiddhā ca,",
  "cnd2:80.2_0": "Vedanaṁ nābhinandato;",
  "cnd2:80.3_0": "Evaṁ satassa carato,",
  "cnd2:80.4_0": "Viññāṇaṁ uparujjhatī”ti.",
  "cnd2:80.5_0": "Udayamāṇavapucchā terasamā.",
  "cnd2:81.0_0": "14. Posālamāṇavapucchā",
  "cnd2:81.1_0": "“Yo atītaṁ ādisati,",
  "cnd2:81.2_0": "(iccāyasmā posālo)",
  "cnd2:81.3_0": "Anejo chinnasaṁsayo;",
  "cnd2:81.4_0": "Pāraguṁ sabbadhammānaṁ,",
  "cnd2:81.5_0": "Atthi pañhena āgamaṁ.",
  "cnd2:82.1_0": "Vibhūtarūpasaññissa,",
  "cnd2:82.2_0": "sabbakāyappahāyino;",
  "cnd2:82.3_0": "Ajjhattañca bahiddhā ca,",
  "cnd2:82.4_0": "natthi kiñcīti passato;",
  "cnd2:82.5_0": "Ñāṇaṁ sakkānupucchāmi,",
  "cnd2:82.6_0": "kathaṁ neyyo tathāvidho”.",
  "cnd2:83.1_0": "“Viññāṇaṭṭhitiyo sabbā,",
  "cnd2:83.2_0": "(posālāti bhagavā)",
  "cnd2:83.3_0": "Abhijānaṁ tathāgato;",
  "cnd2:83.4_0": "Tiṭṭhantamenaṁ jānāti,",
  "cnd2:83.5_0": "Vimuttaṁ tapparāyaṇaṁ.",
  "cnd2:84.1_0": "Ākiñcaññasambhavaṁ ñatvā,",
  "cnd2:84.2_0": "Nandī saṁyojanaṁ iti;",
  "cnd2:84.3_0": "Evametaṁ abhiññāya,",
  "cnd2:84.4_0": "Tato tattha vipassati;",
  "cnd2:84.5_0": "Etaṁ ñāṇaṁ tathaṁ tassa,",
  "cnd2:84.6_0": "Brāhmaṇassa vusīmato”ti.",
  "cnd2:84.7_0": "Posālamāṇavapucchā cuddasamā.",
  "cnd2:85.0_0": "15. Mogharājamāṇavapucchā",
  "cnd2:85.1_0": "“Dvāhaṁ sakkaṁ apucchissaṁ,",
  "cnd2:85.2_0": "(iccāyasmā mogharājā)",
  "cnd2:85.3_0": "Na me byākāsi cakkhumā;",
  "cnd2:85.4_0": "Yāvatatiyañca devīsi,",
  "cnd2:85.5_0": "Byākarotīti me sutaṁ.",
  "cnd2:86.1_0": "Ayaṁ loko paro loko,",
  "cnd2:86.2_0": "brahmaloko sadevako;",
  "cnd2:86.3_0": "Diṭṭhiṁ te nābhijānāti,",
  "cnd2:86.4_0": "gotamassa yasassino.",
  "cnd2:87.1_0": "Evaṁ abhikkantadassāviṁ,",
  "cnd2:87.2_0": "atthi pañhena āgamaṁ;",
  "cnd2:87.3_0": "Kathaṁ lokaṁ avekkhantaṁ,",
  "cnd2:87.4_0": "maccurājā na passati”.",
  "cnd2:88.1_0": "“Suññato lokaṁ avekkhassu,",
  "cnd2:88.2_0": "Mogharāja sadā sato;",
  "cnd2:88.3_0": "Attānudiṭṭhiṁ ūhacca,",
  "cnd2:88.4_0": "Evaṁ maccutaro siyā;",
  "cnd2:88.5_0": "Evaṁ lokaṁ avekkhantaṁ,",
  "cnd2:88.6_0": "Maccurājā na passatī”ti.",
  "cnd2:88.7_0": "Mogharājamāṇavapucchā pannarasamā.",
  "cnd2:89.0_0": "16. Piṅgiyamāṇavapucchā",
  "cnd2:89.1_0": "“Jiṇṇohamasmi abalo vītavaṇṇo,",
  "cnd2:89.2_0": "(iccāyasmā piṅgiyo)",
  "cnd2:89.3_0": "Nettā na suddhā savanaṁ na phāsu;",
  "cnd2:89.4_0": "Māhaṁ nassaṁ momuho antarāva,",
  "cnd2:89.5_0": "Ācikkha dhammaṁ yamahaṁ vijaññaṁ;",
  "cnd2:89.6_0": "Jātijarāya idha vippahānaṁ”.",
  "cnd2:90.1_0": "“Disvāna rūpesu vihaññamāne,",
  "cnd2:90.2_0": "(piṅgiyāti bhagavā)",
  "cnd2:90.3_0": "Ruppanti rūpesu janā pamattā;",
  "cnd2:90.4_0": "Tasmā tuvaṁ piṅgiya appamatto,",
  "cnd2:90.5_0": "Jahassu rūpaṁ apunabbhavāya”.",
  "cnd2:91.1_0": "“Disā catasso vidisā catasso,",
  "cnd2:91.2_0": "Uddhaṁ adho dasa disā imāyo;",
  "cnd2:91.3_0": "Na tuyhaṁ adiṭṭhaṁ asutaṁ amutaṁ,",
  "cnd2:91.4_0": "Atho aviññātaṁ kiñcanamatthi loke;",
  "cnd2:91.5_0": "Ācikkha dhammaṁ yamahaṁ vijaññaṁ,",
  "cnd2:91.6_0": "Jātijarāya idha vippahānaṁ”.",
  "cnd2:92.1_0": "“Taṇhādhipanne manuje pekkhamāno,",
  "cnd2:92.2_0": "(piṅgiyāti bhagavā)",
  "cnd2:92.3_0": "Santāpajāte jarasā parete;",
  "cnd2:92.4_0": "Tasmā tuvaṁ piṅgiya appamatto,",
  "cnd2:92.5_0": "Jahassu taṇhaṁ apunabbhavāyā”ti.",
  "cnd2:92.6_0": "Piṅgiyamāṇavapucchā soḷasamā."
}