{
  "thag3.3:0.1_0": "Theragāthā 3.3",
  "thag3.3:0.2_0": "Tikanipāta",
  "thag3.3:0.3_0": "Paṭhamavagga",
  "thag3.3:0.4_0": "Bākulattheragāthā",
  "thag3.3:1.1_0": "“Yo pubbe karaṇīyāni,",
  "thag3.3:1.2_0": "pacchā so kātumicchati;",
  "thag3.3:1.3_0": "Sukhā so dhaṁsate ṭhānā,",
  "thag3.3:1.4_0": "pacchā ca manutappati.",
  "thag3.3:2.1_0": "Yañhi kayirā tañhi vade,",
  "thag3.3:2.2_0": "yaṁ na kayirā na taṁ vade;",
  "thag3.3:2.3_0": "Akarontaṁ bhāsamānaṁ,",
  "thag3.3:2.4_0": "parijānanti paṇḍitā.",
  "thag3.3:3.1_0": "Susukhaṁ vata nibbānaṁ,",
  "thag3.3:3.2_0": "sammāsambuddhadesitaṁ;",
  "thag3.3:3.3_0": "Asokaṁ virajaṁ khemaṁ,",
  "thag3.3:3.4_0": "yattha dukkhaṁ nirujjhatī”ti.",
  "thag3.3:3.5_0": "… Bākulo thero …."
}