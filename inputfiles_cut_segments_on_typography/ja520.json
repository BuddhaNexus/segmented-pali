{
  "ja520:0.1_0": "Jātaka",
  "ja520:0.2_0": "Tiṁsanipāta",
  "ja520:0.3_0": "Kiṁchandavagga",
  "ja520:0.4_0": "10. Gandhatindukajātaka",
  "ja520:1.1_0": "“Appamādo amataṁ padaṁ,",
  "ja520:1.2_0": "Pamādo maccuno padaṁ;",
  "ja520:1.3_0": "Appamattā na mīyanti,",
  "ja520:1.4_0": "Ye pamattā yathā matā.",
  "ja520:2.1_0": "Madā pamādo jāyetha,",
  "ja520:2.2_0": "pamādā jāyate khayo;",
  "ja520:2.3_0": "Khayā padosā jāyanti,",
  "ja520:2.4_0": "mā pamādo bharatūsabha.",
  "ja520:3.1_0": "Bahū hi khattiyā jīnā,",
  "ja520:3.2_0": "atthaṁ raṭṭhaṁ pamādino;",
  "ja520:3.3_0": "Athopi gāmino gāmā,",
  "ja520:3.4_0": "anagārā agārino.",
  "ja520:4.1_0": "Khattiyassa pamattassa,",
  "ja520:4.2_0": "raṭṭhasmiṁ raṭṭhavaḍḍhana;",
  "ja520:4.3_0": "Sabbe bhogā vinassanti,",
  "ja520:4.4_0": "rañño taṁ vuccate aghaṁ.",
  "ja520:5.1_0": "Nesa dhammo mahārāja,",
  "ja520:5.2_0": "ativelaṁ pamajjasi;",
  "ja520:5.3_0": "Iddhaṁ phītaṁ janapadaṁ,",
  "ja520:5.4_0": "corā viddhaṁsayanti naṁ.",
  "ja520:6.1_0": "Na te puttā bhavissanti,",
  "ja520:6.2_0": "na hiraññaṁ na dhāniyaṁ;",
  "ja520:6.3_0": "Raṭṭhe viluppamānamhi,",
  "ja520:6.4_0": "sabbabhogehi jiyyasi.",
  "ja520:7.1_0": "Sabbabhogā parijiṇṇaṁ,",
  "ja520:7.2_0": "rājānaṁ vāpi khattiyaṁ;",
  "ja520:7.3_0": "Ñātimittā suhajjā ca,",
  "ja520:7.4_0": "na taṁ maññanti māniyaṁ.",
  "ja520:8.1_0": "Hatthārohā anikaṭṭhā,",
  "ja520:8.2_0": "rathikā pattikārakā;",
  "ja520:8.3_0": "Tamevamupajīvantā,",
  "ja520:8.4_0": "na taṁ maññanti māniyaṁ.",
  "ja520:9.1_0": "Asaṁvihitakammantaṁ,",
  "ja520:9.2_0": "bālaṁ dummantimantinaṁ;",
  "ja520:9.3_0": "Sirī jahati dummedhaṁ,",
  "ja520:9.4_0": "jiṇṇaṁva urago tacaṁ.",
  "ja520:10.1_0": "Susaṁvihitakammantaṁ,",
  "ja520:10.2_0": "kāluṭṭhāyiṁ atanditaṁ;",
  "ja520:10.3_0": "Sabbe bhogābhivaḍḍhanti,",
  "ja520:10.4_0": "gāvo sausabhāmiva.",
  "ja520:11.1_0": "Upassutiṁ mahārāja,",
  "ja520:11.2_0": "raṭṭhe janapade cara;",
  "ja520:11.3_0": "Tattha disvā ca sutvā ca,",
  "ja520:11.4_0": "tato taṁ paṭipajjasi”.",
  "ja520:12.1_0": "“Evaṁ vedetu pañcālo,",
  "ja520:12.2_0": "saṅgāme saramappito;",
  "ja520:12.3_0": "Yathāhamajja vedemi,",
  "ja520:12.4_0": "kaṇṭakena samappito”.",
  "ja520:13.1_0": "“Jiṇṇo dubbalacakkhūsi,",
  "ja520:13.2_0": "na rūpaṁ sādhu passasi;",
  "ja520:13.3_0": "Kiṁ tattha brahmadattassa,",
  "ja520:13.4_0": "yaṁ taṁ maggeyya kaṇṭako”.",
  "ja520:14.1_0": "“Bahvettha brahmadattassa,",
  "ja520:14.2_0": "Sohaṁ maggasmi brāhmaṇa;",
  "ja520:14.3_0": "Arakkhitā jānapadā,",
  "ja520:14.4_0": "Adhammabalinā hatā.",
  "ja520:15.1_0": "Rattiñhi corā khādanti,",
  "ja520:15.2_0": "divā khādanti tuṇḍiyā;",
  "ja520:15.3_0": "Raṭṭhasmiṁ kūṭarājassa,",
  "ja520:15.4_0": "bahu adhammiko jano.",
  "ja520:16.1_0": "Etādise bhaye jāte,",
  "ja520:16.2_0": "bhayaṭṭā tāta māṇavā;",
  "ja520:16.3_0": "Nillenakāni kubbanti,",
  "ja520:16.4_0": "vane āhatva kaṇṭakaṁ”.",
  "ja520:17.1_0": "“Kadāssu nāmayaṁ rājā,",
  "ja520:17.2_0": "brahmadatto marissati;",
  "ja520:17.3_0": "Yassa raṭṭhamhi jiyyanti,",
  "ja520:17.4_0": "appatikā kumārikā”.",
  "ja520:18.1_0": "“Dubbhāsitañhi te jammi,",
  "ja520:18.2_0": "anatthapadakovide;",
  "ja520:18.3_0": "Kuhiṁ rājā kumārīnaṁ,",
  "ja520:18.4_0": "bhattāraṁ pariyesati”.",
  "ja520:19.1_0": "“Na me dubbhāsitaṁ brahme,",
  "ja520:19.2_0": "kovidatthapadā ahaṁ;",
  "ja520:19.3_0": "Arakkhitā jānapadā,",
  "ja520:19.4_0": "adhammabalinā hatā.",
  "ja520:20.1_0": "Rattiñhi corā khādanti,",
  "ja520:20.2_0": "divā khādanti tuṇḍiyā;",
  "ja520:20.3_0": "Raṭṭhasmiṁ kūṭarājassa,",
  "ja520:20.4_0": "bahu adhammiko jano;",
  "ja520:20.5_0": "Dujjīve dubbhare dāre,",
  "ja520:20.6_0": "kuto bhattā kumāriyo”.",
  "ja520:21.1_0": "“Evaṁ sayatu pañcālo,",
  "ja520:21.2_0": "saṅgāme sattiyā hato;",
  "ja520:21.3_0": "Yathāyaṁ kapaṇo seti,",
  "ja520:21.4_0": "hato phālena sāliyo”.",
  "ja520:22.1_0": "“Adhammena tuvaṁ jamma,",
  "ja520:22.2_0": "brahmadattassa kujjhasi;",
  "ja520:22.3_0": "Yo tvaṁ sapasi rājānaṁ,",
  "ja520:22.4_0": "aparajjhitvāna attano”.",
  "ja520:23.1_0": "“Dhammena brahmadattassa,",
  "ja520:23.2_0": "ahaṁ kujjhāmi brāhmaṇa;",
  "ja520:23.3_0": "Arakkhitā jānapadā,",
  "ja520:23.4_0": "adhammabalinā hatā.",
  "ja520:24.1_0": "Rattiñhi corā khādanti,",
  "ja520:24.2_0": "divā khādanti tuṇḍiyā;",
  "ja520:24.3_0": "Raṭṭhasmiṁ kūṭarājassa,",
  "ja520:24.4_0": "bahu adhammiko jano.",
  "ja520:25.1_0": "Sā nūna puna re pakkā,",
  "ja520:25.2_0": "vikāle bhattamāhari;",
  "ja520:25.3_0": "Bhattahāriṁ apekkhanto,",
  "ja520:25.4_0": "hato phālena sāliyo”.",
  "ja520:26.1_0": "“Evaṁ haññatu pañcālo,",
  "ja520:26.2_0": "saṅgāme asinā hato;",
  "ja520:26.3_0": "Yathāhamajja pahato,",
  "ja520:26.4_0": "khīrañca me pavaṭṭitaṁ”.",
  "ja520:27.1_0": "“Yaṁ pasu khīraṁ chaḍḍeti,",
  "ja520:27.2_0": "pasupālaṁ vihiṁsati;",
  "ja520:27.3_0": "Kiṁ tattha brahmadattassa,",
  "ja520:27.4_0": "yaṁ no garahate bhavaṁ”.",
  "ja520:28.1_0": "“Gārayho brahme pañcālo,",
  "ja520:28.2_0": "brahmadattassa rājino;",
  "ja520:28.3_0": "Arakkhitā jānapadā,",
  "ja520:28.4_0": "adhammabalinā hatā.",
  "ja520:29.1_0": "Rattiñhi corā khādanti,",
  "ja520:29.2_0": "divā khādanti tuṇḍiyā;",
  "ja520:29.3_0": "Raṭṭhasmiṁ kūṭarājassa,",
  "ja520:29.4_0": "bahu adhammiko jano.",
  "ja520:30.1_0": "Caṇḍā aṭanakā gāvī,",
  "ja520:30.2_0": "yaṁ pure na duhāmase;",
  "ja520:30.3_0": "Taṁ dāni ajja dohāma,",
  "ja520:30.4_0": "khīrakāmehupaddutā”.",
  "ja520:31.1_0": "“Evaṁ kandatu pañcālo,",
  "ja520:31.2_0": "viputto vippasukkhatu;",
  "ja520:31.3_0": "Yathāyaṁ kapaṇā gāvī,",
  "ja520:31.4_0": "viputtā paridhāvati”.",
  "ja520:32.1_0": "“Yaṁ pasu pasupālassa,",
  "ja520:32.2_0": "sambhameyya raveyya vā;",
  "ja520:32.3_0": "Ko nīdha aparādhatthi,",
  "ja520:32.4_0": "brahmadattassa rājino”.",
  "ja520:33.1_0": "“Aparādho mahābrahme,",
  "ja520:33.2_0": "brahmadattassa rājino;",
  "ja520:33.3_0": "Arakkhitā jānapadā,",
  "ja520:33.4_0": "adhammabalinā hatā.",
  "ja520:34.1_0": "Rattiñhi corā khādanti,",
  "ja520:34.2_0": "divā khādanti tuṇḍiyā;",
  "ja520:34.3_0": "Raṭṭhasmiṁ kūṭarājassa,",
  "ja520:34.4_0": "bahu adhammiko jano;",
  "ja520:34.5_0": "Kathaṁ no asikosatthā,",
  "ja520:34.6_0": "khīrapā haññate pajā”.",
  "ja520:35.1_0": "“Evaṁ khajjatu pañcālo,",
  "ja520:35.2_0": "hato yuddhe saputtako;",
  "ja520:35.3_0": "Yathāhamajja khajjāmi,",
  "ja520:35.4_0": "gāmikehi araññajo”.",
  "ja520:36.1_0": "“Na sabbabhūtesu vidhenti rakkhaṁ,",
  "ja520:36.2_0": "Rājāno maṇḍūka manussaloke;",
  "ja520:36.3_0": "Nettāvatā rājā adhammacārī,",
  "ja520:36.4_0": "Yaṁ tādisaṁ jīvamadeyyu dhaṅkā”.",
  "ja520:37.1_0": "“Adhammarūpo vata brahmacārī,",
  "ja520:37.2_0": "Anuppiyaṁ bhāsasi khattiyassa;",
  "ja520:37.3_0": "Viluppamānāya puthuppajāya,",
  "ja520:37.4_0": "Pūjesi rājaṁ paramappamādaṁ.",
  "ja520:38.1_0": "Sace idaṁ brahme surajjakaṁ siyā,",
  "ja520:38.2_0": "Phītaṁ raṭṭhaṁ muditaṁ vippasannaṁ;",
  "ja520:38.3_0": "Bhutvā baliṁ aggapiṇḍañca kākā,",
  "ja520:38.4_0": "Na mādisaṁ jīvamadeyyu dhaṅkā”ti.",
  "ja520:39.1_0": "Gandhatindukajātakaṁ dasamaṁ.",
  "ja520:40.1_0": "Tiṁsanipātaṁ niṭṭhitaṁ.",
  "ja520:41.1_0": "Tassuddānaṁ",
  "ja520:42.1_0": "Kiṁchanda kumbha jayaddisa chaddanta,",
  "ja520:42.2_0": "Atha paṇḍitasambhava sirakapi;",
  "ja520:42.3_0": "Dakarakkhasa paṇḍaranāgavaro,",
  "ja520:42.4_0": "Atha sambula tindukadevasutoti.",
  "ja520:43.1_0": "Jātakapāḷiyā paṭhamo bhāgo niṭṭhito."
}