{
  "snp1.12:0.1_0": "Sutta Nipāta 1.12",
  "snp1.12:0.2_0": "Munisutta",
  "snp1.12:1.1_0": "Santhavāto bhayaṁ jātaṁ,",
  "snp1.12:1.2_0": "niketā jāyate rajo;",
  "snp1.12:1.3_0": "Aniketamasanthavaṁ,",
  "snp1.12:1.4_0": "etaṁ ve munidassanaṁ.",
  "snp1.12:2.1_0": "Yo jātamucchijja na ropayeyya,",
  "snp1.12:2.2_0": "Jāyantamassa nānuppavecche;",
  "snp1.12:2.3_0": "Tamāhu ekaṁ muninaṁ carantaṁ,",
  "snp1.12:2.4_0": "Addakkhi so santipadaṁ mahesi.",
  "snp1.12:3.1_0": "Saṅkhāya vatthūni pamāya bījaṁ,",
  "snp1.12:3.2_0": "Sinehamassa nānuppavecche;",
  "snp1.12:3.3_0": "Sa ve munī jātikhayantadassī,",
  "snp1.12:3.4_0": "Takkaṁ pahāya na upeti saṅkhaṁ.",
  "snp1.12:4.1_0": "Aññāya sabbāni nivesanāni,",
  "snp1.12:4.2_0": "Anikāmayaṁ aññatarampi tesaṁ;",
  "snp1.12:4.3_0": "Sa ve munī vītagedho agiddho,",
  "snp1.12:4.4_0": "Nāyūhatī pāragato hi hoti.",
  "snp1.12:5.1_0": "Sabbābhibhuṁ sabbaviduṁ sumedhaṁ,",
  "snp1.12:5.2_0": "Sabbesu dhammesu anūpalittaṁ;",
  "snp1.12:5.3_0": "Sabbañjahaṁ taṇhakkhaye vimuttaṁ,",
  "snp1.12:5.4_0": "Taṁ vāpi dhīrā muni vedayanti.",
  "snp1.12:6.1_0": "Paññābalaṁ sīlavatūpapannaṁ,",
  "snp1.12:6.2_0": "Samāhitaṁ jhānarataṁ satīmaṁ;",
  "snp1.12:6.3_0": "Saṅgā pamuttaṁ akhilaṁ anāsavaṁ,",
  "snp1.12:6.4_0": "Taṁ vāpi dhīrā muni vedayanti.",
  "snp1.12:7.1_0": "Ekaṁ carantaṁ munimappamattaṁ,",
  "snp1.12:7.2_0": "Nindāpasaṁsāsu avedhamānaṁ;",
  "snp1.12:7.3_0": "Sīhaṁva saddesu asantasantaṁ,",
  "snp1.12:7.4_0": "Vātaṁva jālamhi asajjamānaṁ;",
  "snp1.12:7.5_0": "Padmaṁva toyena alippamānaṁ,",
  "snp1.12:7.6_0": "Netāramaññesamanaññaneyyaṁ;",
  "snp1.12:7.7_0": "Taṁ vāpi dhīrā muni vedayanti.",
  "snp1.12:8.1_0": "Yo ogahaṇe thambhorivābhijāyati,",
  "snp1.12:8.2_0": "Yasmiṁ pare vācāpariyantaṁ vadanti;",
  "snp1.12:8.3_0": "Taṁ vītarāgaṁ susamāhitindriyaṁ,",
  "snp1.12:8.4_0": "Taṁ vāpi dhīrā muni vedayanti.",
  "snp1.12:9.1_0": "Yo ve ṭhitatto tasaraṁva ujju,",
  "snp1.12:9.2_0": "Jigucchati kammehi pāpakehi;",
  "snp1.12:9.3_0": "Vīmaṁsamāno visamaṁ samañca,",
  "snp1.12:9.4_0": "Taṁ vāpi dhīrā muni vedayanti.",
  "snp1.12:10.1_0": "Yo saññatatto na karoti pāpaṁ,",
  "snp1.12:10.2_0": "Daharo majjhimo ca muni yatatto;",
  "snp1.12:10.3_0": "Arosaneyyo na so roseti kañci,",
  "snp1.12:10.4_0": "Taṁ vāpi dhīrā muni vedayanti.",
  "snp1.12:11.1_0": "Yadaggato majjhato sesato vā,",
  "snp1.12:11.2_0": "Piṇḍaṁ labhetha paradattūpajīvī;",
  "snp1.12:11.3_0": "Nālaṁ thutuṁ nopi nipaccavādī,",
  "snp1.12:11.4_0": "Taṁ vāpi dhīrā muni vedayanti.",
  "snp1.12:12.1_0": "Muniṁ carantaṁ virataṁ methunasmā,",
  "snp1.12:12.2_0": "Yo yobbane nopanibajjhate kvaci;",
  "snp1.12:12.3_0": "Madappamādā virataṁ vippamuttaṁ,",
  "snp1.12:12.4_0": "Taṁ vāpi dhīrā muni vedayanti.",
  "snp1.12:13.1_0": "Aññāya lokaṁ paramatthadassiṁ,",
  "snp1.12:13.2_0": "Oghaṁ samuddaṁ atitariya tādiṁ;",
  "snp1.12:13.3_0": "Taṁ chinnaganthaṁ asitaṁ anāsavaṁ,",
  "snp1.12:13.4_0": "Taṁ vāpi dhīrā muni vedayanti.",
  "snp1.12:14.1_0": "Asamā ubho dūravihāravuttino,",
  "snp1.12:14.2_0": "Gihī dāraposī amamo ca subbato;",
  "snp1.12:14.3_0": "Parapāṇarodhāya gihī asaññato,",
  "snp1.12:14.4_0": "Niccaṁ munī rakkhati pāṇine yato.",
  "snp1.12:15.1_0": "Sikhī yathā nīlagīvo vihaṅgamo,",
  "snp1.12:15.2_0": "Haṁsassa nopeti javaṁ kudācanaṁ;",
  "snp1.12:15.3_0": "Evaṁ gihī nānukaroti bhikkhuno,",
  "snp1.12:15.4_0": "Munino vivittassa vanamhi jhāyatoti.",
  "snp1.12:15.5_0": "Munisuttaṁ dvādasamaṁ.",
  "snp1.12:15.6_0": "Uragavaggo paṭhamo.",
  "snp1.12:16.0_0": "Tassuddānaṁ",
  "snp1.12:16.1_0": "Urago dhaniyo ceva,",
  "snp1.12:16.2_0": "visāṇañca tathā kasi;",
  "snp1.12:16.3_0": "Cundo parābhavo ceva,",
  "snp1.12:16.4_0": "vasalo mettabhāvanā.",
  "snp1.12:17.1_0": "Sātāgiro āḷavako,",
  "snp1.12:17.2_0": "Vijayo ca tathā muni;",
  "snp1.12:17.3_0": "Dvādasetāni suttāni,",
  "snp1.12:17.4_0": "Uragavaggoti vuccatīti."
}