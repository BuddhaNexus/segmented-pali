{
  "anya-e0701n1:0_0": "2. Ganthakārakācariya-paricchedo",
  "anya-e0701n1:1_0": "Ācariyo pana atthi.",
  "anya-e0701n1:1_1": "Porāṇācariyā atthi,",
  "anya-e0701n1:1_2": "aṭṭhakathācariyā atthi,",
  "anya-e0701n1:1_3": "ganthakārakācariyā atthi,",
  "anya-e0701n1:1_4": "tividhanāmikācariyā.",
  "anya-e0701n1:1_5": "Katame porāṇācariyā?",
  "anya-e0701n1:1_6": "Paṭhamasaṅgāyanāyaṁ pañcasatā khīṇāsavā pañcannaṁ nikāyānaṁ nāmañca atthañca adhippāyañca yadañca byañjana sodhanañca avasesaṁ kiccaṁ kariṁsu.",
  "anya-e0701n1:1_7": "Dutiyasaṅgāyanāyaṁ sattasatā khīṇāsavā tesaṁyeva saddatthādikaṁ kiccaṁ puna kariṁsu.",
  "anya-e0701n1:1_8": "Tatiyasaṅgāyanāyaṁ sahassamattā khīṇāsavā tesaṁyeva saddatthādikaṁ kiccaṁ puna kariṁsu.",
  "anya-e0701n1:1_9": "Iccevaṁ dvesatādhikā dvesahassa khīṇāsavā mahākaccāyanaṁ ṭhapetvā avasesā porāṇācariyā nāma.",
  "anya-e0701n1:1_10": "Ye porāṇācariyā teyeva aṭṭhakathācariyā nāma.",
  "anya-e0701n1:1_11": "Katame ganthakārakācariyā?",
  "anya-e0701n1:1_12": "Mahāaṭṭhakathikāpebhadaanekācariyā ganthakārakācariyā nāma.",
  "anya-e0701n1:1_13": "Katame tividha nāmācariyā mahākaccāyano tividhanāmaṁ.",
  "anya-e0701n1:1_14": "Katame ganthā mahākaccāyanena katā?",
  "anya-e0701n1:1_15": "Kaccāyanagantho,",
  "anya-e0701n1:1_16": "mahāniruttigantho,",
  "anya-e0701n1:1_17": "cullaniruttigantho,",
  "anya-e0701n1:1_18": "yamakagantho,",
  "anya-e0701n1:1_19": "nettigantho,",
  "anya-e0701n1:1_20": "peṭakopadesaganthoti,",
  "anya-e0701n1:1_21": "ime cha ganthā mahākaccāyanena katā.",
  "anya-e0701n1:1_22": "Katame anekācariyena katā ganthā?",
  "anya-e0701n1:1_23": "Mahāpaccarikācariyo mahāpaccariyaṁ nāma ganthaṁ akāsi.",
  "anya-e0701n1:1_24": "Mahākurundikācariyo kurundi nāma ganthaṁ akāsi.",
  "anya-e0701n1:1_25": "Aññataro ācariyo mahāpaccariya ganthassa aṭṭhakathaṁ akāsi.",
  "anya-e0701n1:1_26": "Aññataro ācariyo kurundi ganthassa aṭṭhakathaṁ akāsi,",
  "anya-e0701n1:1_27": "mahābuddhaghosā nāmacariyo visuddhimaggo dīghanikāyassa sumaṅgalavilāsani nāma aṭṭhakathā,",
  "anya-e0701n1:1_28": "majjhimanikāyassa papañcasūdanī nāma aṭṭhakathā,",
  "anya-e0701n1:1_29": "saṁyuttanikāyassa sāratthappakāsinī nāma aṭṭhakathā,",
  "anya-e0701n1:1_30": "aṅguttaranikāyassa manorathapūraṇī nāma aṭṭhakathā,",
  "anya-e0701n1:1_31": "pañcavinaya ganthānaṁ samantapāsādikā nāma aṭṭhakathā,",
  "anya-e0701n1:1_32": "sattannaṁ abhidhammaganthānaṁ paramatthakathā nāma aṭṭhakathā,",
  "anya-e0701n1:1_33": "pātimokkha saṁkhātāya mātikāya kaṅkhāvitaraṇīti visuddhi nāma aṭṭhakathā,",
  "anya-e0701n1:1_34": "dhammapadassa aṭṭhakathā,",
  "anya-e0701n1:1_35": "jātakassa aṭṭhakathā,",
  "anya-e0701n1:1_36": "khuddakapāṭhassa aṭṭhakathā,",
  "anya-e0701n1:1_37": "suttanipātassa aṭṭhakathā,",
  "anya-e0701n1:1_38": "apadānassa aṭṭhakathāti,",
  "anya-e0701n1:1_39": "ime terasa ganthe akāsi.",
  "anya-e0701n1:1_40": "Buddhadattonāmācariyo vinaya vinicchayo,",
  "anya-e0701n1:1_41": "uttaravinicchayo,",
  "anya-e0701n1:1_42": "abhidhammāvatāro,",
  "anya-e0701n1:1_43": "buddhavaṁsassa madhuratthavilāsinī nāma aṭṭhakathāti,",
  "anya-e0701n1:1_44": "ime cattāro ganthe akāsi.",
  "anya-e0701n1:1_45": "Ānandonāmācariyā sattābhidhammaganthaṭṭhakathāya mūṭīkaṁ nāma ṭīkaṁ akāsi.",
  "anya-e0701n1:1_46": "Dhammapālācariyo nettippakaraṇaṭṭhakathā,",
  "anya-e0701n1:1_47": "itivuttakaṭṭhakathā,",
  "anya-e0701n1:1_48": "udānaṭṭhakathā,",
  "anya-e0701n1:1_49": "cariyāpiṭakaṭṭhakathā,",
  "anya-e0701n1:1_50": "therakathaṭṭhakathā,",
  "anya-e0701n1:1_51": "therīkathaṭṭhakathā,",
  "anya-e0701n1:1_52": "vimānavatthussa vimalavilāsini nāma aṭṭhakathā,",
  "anya-e0701n1:1_53": "petavatthussa vimalavilāsini nāma aṭṭhakathā,",
  "anya-e0701n1:1_54": "visuddhimaggassa paramatthamañjūsā nāma ṭīkā,",
  "anya-e0701n1:1_55": "dīghanikāyassa aṭṭhakathādīnaṁ catunnaṁ aṭṭhakathānaṁ līnatthappakāsani nāma ṭīkā,",
  "anya-e0701n1:1_56": "jātakaṭṭhakathāya līnatthappakāsani nāma ṭīkā,",
  "anya-e0701n1:1_57": "nettipakaraṇaṭṭhakathāya ṭīkā,",
  "anya-e0701n1:1_58": "buddhavaṁsaṭṭhakathāya paramatthadīpanī nāma ṭīkā,",
  "anya-e0701n1:1_59": "abhidhammaṭṭhakathāyaṭīkā līnatthavaṇṇanā nāma anuṭīkāti ime cuddasa matte ganthe akāsi.",
  "anya-e0701n1:2_0": "Dve pubbācariyānāmā cariyānirutti mañjūsaṁ nāma cullanirutti ṭīkañca mahānirutti saṅkhepañca akaṁsu.",
  "anya-e0701n1:2_1": "Mahāvajirabuddhināmācariyo vinayagaṇṭhināma pakaraṇaṁ akāsi.",
  "anya-e0701n1:2_2": "Dīpaṅkarasaṅkhāto vimalabuddhi nāmācariyo mukhamattadīpanī nāmakaṁ nyāsappakaraṇaṁ akāsi.",
  "anya-e0701n1:2_3": "Cullavajirabuddhi nāmācariyo atthabyākhyānaṁ nāma pakaraṇaṁ akāsi.",
  "anya-e0701n1:3_0": "Dīpaṅkaro nāmācariyo rūpasiddhi pakaraṇaṁ,",
  "anya-e0701n1:3_1": "rūpasiddhi ṭīkaṁ,",
  "anya-e0701n1:3_2": "sampapañca suttañceti tividhaṁ pakaraṇaṁ akāsi.",
  "anya-e0701n1:3_3": "Ānandācariyassa jeṭṭhasisso dhammapālo nāmācariyo saccasaṅkhepaṁ nāma pakaraṇaṁ akāsi.",
  "anya-e0701n1:3_4": "Kassapo nāmācariyo mohavicchedanī,",
  "anya-e0701n1:3_5": "vimaticchedanī,",
  "anya-e0701n1:3_6": "dasabuddhavaṁso,",
  "anya-e0701n1:3_7": "anāgatavaṁsoti,",
  "anya-e0701n1:3_8": "catuvidhaṁ pakaraṇaṁ akāsi.",
  "anya-e0701n1:4_0": "Mahānāmo nāmācariyo,",
  "anya-e0701n1:4_1": "saddhammapakāsanī nāma paṭisambhidāmaggassa aṭṭhakathaṁ akāsi.",
  "anya-e0701n1:4_2": "Dīpavaṁso,",
  "anya-e0701n1:4_3": "thūpavaṁso,",
  "anya-e0701n1:4_4": "bodhivaṁso,",
  "anya-e0701n1:4_5": "cūlavaṁso,",
  "anya-e0701n1:4_6": "mahāvaṁso,",
  "anya-e0701n1:4_7": "paṭisambhidāmaggaṭṭhakathā gaṇṭhi ceti ime cha ganthā mahānāmācari visuṁ visuṁ katā.",
  "anya-e0701n1:5_0": "Navo mahānāmo nāmācariyo navaṁ mahāvaṁsa nāma pakaraṇaṁ akāsi.",
  "anya-e0701n1:5_1": "Upaseno nāmācariyo saddhammapajjotikaṁ nāma mahāniddesassa aṭṭhakathaṁ akāsi.",
  "anya-e0701n1:5_2": "Moggalāno nāmācariyo moggalānabyākaraṇaṁ nāma pakaraṇaṁ akāsi.",
  "anya-e0701n1:5_3": "Saṅgharakkhito nāmācariyo,",
  "anya-e0701n1:5_4": "subodhālaṅkāraṁ nāma pakaraṇaṁ akāsi.",
  "anya-e0701n1:5_5": "Vuttodayakāro nāmācariyo vuttodayaṁ nāma pakaraṇaṁ akāsi.",
  "anya-e0701n1:5_6": "Dhammasiri nāmācariyo khuddakasikkhaṁ nāma pakaraṇaṁ akāsi.",
  "anya-e0701n1:5_7": "Purāṇakhuddasikkhāya ṭīkā,",
  "anya-e0701n1:5_8": "mūlasikkhā ceti,",
  "anya-e0701n1:5_9": "ime dve ganthā dvehācariyehi visuṁ visuṁ katā.",
  "anya-e0701n1:6_0": "Anuruddho nāmācariyo paramatthavinicchayaṁ,",
  "anya-e0701n1:6_1": "nāmarūpaparicchedaṁ,",
  "anya-e0701n1:6_2": "abhidhammatthasaṅgahaṁ ceti tividhaṁ pakaraṇaṁ akāsi.",
  "anya-e0701n1:6_3": "Khemo nāmācariyo khemaṁ nāma pakaraṇaṁ akāsi.",
  "anya-e0701n1:6_4": "Sāriputto nāmācariyo vinayaṭṭhakathāya sāratthadīpanī nāma ṭīkaṁ;",
  "anya-e0701n1:6_5": "vinayasaṅgahapakaraṇaṁ,",
  "anya-e0701n1:6_6": "vinayasaṅgahassaṭīkaṁ;",
  "anya-e0701n1:6_7": "aṅguttaraṭṭhakathāya sāratthamañjūsaṁ nāma navaṁ ṭīkaṁ;",
  "anya-e0701n1:6_8": "pañcikā ṭīkañceti,",
  "anya-e0701n1:6_9": "ime pañca ganthe akāsi.",
  "anya-e0701n1:6_10": "Buddhanāgo nāmācariyo vinayatthamañjūsaṁ nāma kaṅkhāvitaraṇīyā ṭīkaṁ akāsi.",
  "anya-e0701n1:6_11": "Navo moggalāno nāmācariyo abhidhānappadīpikaṁ nāma pakaraṇaṁ akāsi.",
  "anya-e0701n1:6_12": "Vācissaro nāmācariyo mahāsāmi nāma subodhālaṅkārassa ṭīkā,",
  "anya-e0701n1:6_13": "vuttodaya vivaraṇaṁ,",
  "anya-e0701n1:6_14": "sumaṅgalappasādani nāma khuddasikkhāya ṭīkā;",
  "anya-e0701n1:6_15": "sambandhacintā,",
  "anya-e0701n1:6_16": "sambandhacintāya ṭīkā;",
  "anya-e0701n1:6_17": "bālāvatāro,",
  "anya-e0701n1:6_18": "moggalānabyākaraṇassa pañcikāya ṭīkā;",
  "anya-e0701n1:6_19": "yogavinicchayo,",
  "anya-e0701n1:6_20": "vinayavinicchayassa ṭīkā,",
  "anya-e0701n1:6_21": "uttaravinicchayassa ṭīkā, nāmarūpa-",
  "anya-e0701n1:6_22": "paricchedassa vibhāgo,",
  "anya-e0701n1:6_23": "saddatthassa padarūpavibhāvanaṁ;",
  "anya-e0701n1:6_24": "khemassa pakaraṇassa ṭīkā,",
  "anya-e0701n1:6_25": "sīmālaṅkāro,",
  "anya-e0701n1:6_26": "mūlasikkhāya ṭīkā,",
  "anya-e0701n1:6_27": "rūpavibhāgo,",
  "anya-e0701n1:6_28": "paccayasaṅgaho,",
  "anya-e0701n1:6_29": "saccasaṅkhepassa ṭīkā ceti,",
  "anya-e0701n1:6_30": "ime aṭṭhārasa ganthe akāsi.",
  "anya-e0701n1:7_0": "Sumaṅgalo nāmācariyo abhidhammāvatārassaṭīkaṁ,",
  "anya-e0701n1:7_1": "abhidhammatthasaṅgahassaṭīkañca duvidhaṁ pakaraṇaṁ akāsi.",
  "anya-e0701n1:7_2": "Buddhapiyo nāmācariyo sāratthasaṅgahaṁ nāma pakaraṇaṁ akāsi.",
  "anya-e0701n1:7_3": "Dhammakitti nāmācariyo dantadhātu pakaraṇaṁ akāsi.",
  "anya-e0701n1:7_4": "Medhaṅkaro nāmācariyo jinacaritaṁ nāma pakaraṇaṁ akāsi.",
  "anya-e0701n1:7_5": "Buddharakkhito nāmācariyo jinalaṅkāraṁ,",
  "anya-e0701n1:7_6": "jinalaṅkārassa ṭīkañcāti duvidhaṁ pakaraṇaṁ akāsi.",
  "anya-e0701n1:7_7": "Upatisso nāmācariyo anāgatavaṁsassa aṭṭhakathaṁ akāsi.",
  "anya-e0701n1:8_0": "Kaṅkhāvitaraṇīyā līnatthappakāsini,",
  "anya-e0701n1:8_1": "nisandeho,",
  "anya-e0701n1:8_2": "dhammānusāraṇī,",
  "anya-e0701n1:8_3": "ñeyyāsantati,",
  "anya-e0701n1:8_4": "ñeyyāsantatiyā ṭīkā,",
  "anya-e0701n1:8_5": "sumatādāvatāro,",
  "anya-e0701n1:8_6": "lokapaññatti pakaraṇaṁ,",
  "anya-e0701n1:8_7": "tathāgatuppatti pakaraṇaṁ,",
  "anya-e0701n1:8_8": "nalāṭadhātu vaṇṇanā,",
  "anya-e0701n1:8_9": "sīhaḷavatthu,",
  "anya-e0701n1:8_10": "dhammadīpako,",
  "anya-e0701n1:8_11": "paṭipattisaṅgaho,",
  "anya-e0701n1:8_12": "visuddhimaggaṇṭhi,",
  "anya-e0701n1:8_13": "abhidhammagaṇṭhi,",
  "anya-e0701n1:8_14": "nettipakaraṇagaṇṭhi,",
  "anya-e0701n1:8_15": "visuddhimaggacullanavaṭīkā,",
  "anya-e0701n1:8_16": "sotabbamālinī,",
  "anya-e0701n1:8_17": "pasādajananī,",
  "anya-e0701n1:8_18": "okāsaloko,",
  "anya-e0701n1:8_19": "subodhālaṅkārassa nava ṭīkā ceti,",
  "anya-e0701n1:8_20": "ime vīsati ganthā vīsatācariyehi visuṁ visuṁ katā.",
  "anya-e0701n1:9_0": "Saddhammasiri nāmācariyo saddatthabhedacintā nāma pakaraṇaṁ akāsi.",
  "anya-e0701n1:9_1": "Devo nāmācariyo sumana kūṭavaṇṇanaṁ nāma pakaraṇaṁ akāsi.",
  "anya-e0701n1:9_2": "Cullabuddhaghoso nāmācariyo sotatthakinidānaṁ nāma pakaraṇaṁ akāsi.",
  "anya-e0701n1:9_3": "Raṭṭhapālo nāmācariyo madhurasaṅgāhaṇīkitti nāma pakaraṇaṁ akāsi.",
  "anya-e0701n1:9_4": "Subhūtacando nāmācariyo liṅgatthavivaraṇa-",
  "anya-e0701n1:9_5": "pakaraṇaṁ akāsi.",
  "anya-e0701n1:9_6": "Aggavaṁso nāmācariyo saddanīti pakaraṇaṁ nāma akāsi.",
  "anya-e0701n1:9_7": "Vajirabuddhi nāmācariyo mahāṭīkaṁ nāma nyāsapakaraṇaṭīkaṁ akāsi.",
  "anya-e0701n1:9_8": "Guṇasāgaro nāmācariyo mukhamattasāraṁ,",
  "anya-e0701n1:9_9": "mukhamattasārassa ṭīkañca duvidhaṁ pakaraṇaṁ akāsi.",
  "anya-e0701n1:9_10": "Abhayo nāmācariyo saddatthabhedacintāya mahāṭīkaṁ akāsi.",
  "anya-e0701n1:9_11": "Ñāṇasāgaro nāmācariyo liṅgatthavivaraṇappakāsanaṁ nāma pakaraṇaṁ akāsi.",
  "anya-e0701n1:9_12": "Aññataro ācariyo gūḷatthaṭīkaṁ,",
  "anya-e0701n1:9_13": "bālappabodhanañca duvidhaṁ pakaraṇaṁ akāsi.",
  "anya-e0701n1:9_14": "Aññataro ācariyo saddattha-",
  "anya-e0701n1:9_15": "bhedacintāya majjhimaṭīkaṁ akāsi.",
  "anya-e0701n1:9_16": "Uttamo nāmācariyo bālāvatāraṭīkaṁ,",
  "anya-e0701n1:9_17": "liṅgatthavivaraṇaṭīkañca duvidhaṁ pakaraṇaṁ akāsi.",
  "anya-e0701n1:9_18": "Aññataro ācariyo saddatthabhedacintāya nava-",
  "anya-e0701n1:9_19": "ṭīkaṁ akāsi.",
  "anya-e0701n1:9_20": "Eko amacco abhidhānappadīpikāyaṭīkaṁ,",
  "anya-e0701n1:9_21": "gaṇṭhipakaraṇassa daṇḍīppakaraṇassa māgadhabhūtaṁ ṭīkaṁ,",
  "anya-e0701n1:9_22": "koladdhajanassa sakaṭabhāsāya kataṭīkañca tividhaṁ pakaraṇaṁ akāsi.",
  "anya-e0701n1:9_23": "Dhammasenāpati nāmācariyo kārikaṁ,",
  "anya-e0701n1:9_24": "etimāsapidīpanī,",
  "anya-e0701n1:9_25": "manohārañca tividhaṁ pakaraṇaṁ akāsi.",
  "anya-e0701n1:9_26": "Aññataro ācariyo kārikāya ṭīkaṁ akāsi.",
  "anya-e0701n1:9_27": "Aññataro ācariyo etimāsapidīpikāya ṭīkaṁ akāsi.",
  "anya-e0701n1:10_0": "Aññataro ācariyo saddabindu nāma pakaraṇaṁ akāsi.",
  "anya-e0701n1:10_1": "Saddhammaguru nāmācariyo saddavuttippakāsakaṁ nāma pakaraṇaṁ akāsi.",
  "anya-e0701n1:10_2": "Sāriputto nāmācariyo saddavuttippakāsakassa ṭīkaṁ akāsi.",
  "anya-e0701n1:10_3": "Aññataro ācariyo kaccāyanasāraṁ nāma pakaraṇaṁ kaccāyanasārassa ṭīkañca duvidhaṁ pakaraṇaṁ akāsi.",
  "anya-e0701n1:10_4": "Navo medhaṅkaro nāmācariyo lokadīpakasāraṁ nāma pakaraṇaṁ akāsi.",
  "anya-e0701n1:10_5": "Aggapaṇḍito nāmācariyo lokuppatti nāma pakaraṇaṁ akāsi.",
  "anya-e0701n1:10_6": "Cīvaro nāmācariyo jaṅghadāsakassa ṭīkaṁ akāsi.",
  "anya-e0701n1:10_7": "Mātikatthadīpanī,",
  "anya-e0701n1:10_8": "abhidhammatthasaṅgahavaṇṇanā,",
  "anya-e0701n1:10_9": "sīmālaṅkārassaṭīkā,",
  "anya-e0701n1:10_10": "vinayasamuṭṭhānadīpanī ṭīkā,",
  "anya-e0701n1:10_11": "gaṇṭhi sāro,",
  "anya-e0701n1:10_12": "paṭṭhānagaṇanā nayo,",
  "anya-e0701n1:10_13": "suttaniddeso,",
  "anya-e0701n1:10_14": "pātimokkho, ceti,",
  "anya-e0701n1:10_15": "ime aṭṭha ganthe saddhammajotipālācariyo akāsi.",
  "anya-e0701n1:10_16": "Vimalabuddhi nāmācariyo abhidhamma-",
  "anya-e0701n1:10_17": "pannarasaṭṭhānaṁ nāma pakaraṇaṁ akāsi.",
  "anya-e0701n1:10_18": "Navo vimalabuddhi nāmācariyo saddasāratthajālinī,",
  "anya-e0701n1:10_19": "saddasāratthajāliniyā ṭīkā,",
  "anya-e0701n1:10_20": "vuttodaya ṭīkā,",
  "anya-e0701n1:10_21": "paramatthamañjūsā nāma abhidhammasaṅgahaṭīkāya anuṭīkā dasagaṇṭhivaṇṇanā,",
  "anya-e0701n1:10_22": "māgadhabhūtāvidaggamukhamaṇḍanaṭīkā ceti ime cha ganthe akāsi.",
  "anya-e0701n1:10_23": "Aññataro ācariyo pañcapakaraṇaṭīkāya navānuṭīkaṁ akāsi.",
  "anya-e0701n1:10_24": "Ariyavaṁso nāmācariyo abhidhammasaṅgaha-",
  "anya-e0701n1:10_25": "ṭīkāya [paramattha] maṇisāramañjūsaṁ nāma navānuṭīkaṁ akāsi.",
  "anya-e0701n1:10_26": "Abhidhammatthasaṅgahassa ṭīkā,",
  "anya-e0701n1:10_27": "peṭakopadesassa ṭīkā,",
  "anya-e0701n1:10_28": "catubhāṇavārassa aṭṭhakathā,",
  "anya-e0701n1:10_29": "mahāsārapakāsanī,",
  "anya-e0701n1:10_30": "mahādīpanī,",
  "anya-e0701n1:10_31": "sāratthadīpanī gati pakaraṇaṁ,",
  "anya-e0701n1:10_32": "hatthasāro,",
  "anya-e0701n1:10_33": "bhummasaṅgaho,",
  "anya-e0701n1:10_34": "bhummaniddeso,",
  "anya-e0701n1:10_35": "dasavatthukāyaviratiṭīkā,",
  "anya-e0701n1:10_36": "jotanā nirutti,",
  "anya-e0701n1:10_37": "vibhattikathā,",
  "anya-e0701n1:10_38": "kaccāyanavivaraṇā,",
  "anya-e0701n1:10_39": "saddhammamālinī,",
  "anya-e0701n1:10_40": "pañcagati vaṇṇanā,",
  "anya-e0701n1:10_41": "bālacittapabodhanaṁ,",
  "anya-e0701n1:10_42": "dhammacakkasuttassa navaṭṭhakathā,",
  "anya-e0701n1:10_43": "dantadhātu pakaraṇassa ṭīkā ceti,",
  "anya-e0701n1:10_44": "ime vīsati ganthā nānācariyehi katā,",
  "anya-e0701n1:10_45": "aññāni pana pakaraṇāni atthi.",
  "anya-e0701n1:11_0": "Katamāni saddhammo pāyano,",
  "anya-e0701n1:11_1": "bālappabodhanapakaraṇassa ṭīkā ca,",
  "anya-e0701n1:11_2": "jinālaṅkārapakaraṇassa navaṭīkā ca,",
  "anya-e0701n1:11_3": "liṅgatthavivaraṇaṁ,",
  "anya-e0701n1:11_4": "liṅgavinicchayo;",
  "anya-e0701n1:11_5": "pātimokkhavivaraṇaṁ,",
  "anya-e0701n1:11_6": "paramatthakathāvivaraṇaṁ,",
  "anya-e0701n1:11_7": "samantapāsādikā vivaraṇaṁ,",
  "anya-e0701n1:11_8": "catubhāṇavāraṭṭhakathā vivaraṇaṁ,",
  "anya-e0701n1:11_9": "abhidhammatthasaṅgahavivaraṇaṁ,",
  "anya-e0701n1:11_10": "saccasaṅkhepavivaraṇaṁ,",
  "anya-e0701n1:11_11": "saddatthabhedacintāvivaraṇaṁ,",
  "anya-e0701n1:11_12": "saddavuttivivaraṇaṁ,",
  "anya-e0701n1:11_13": "kaccāyanasāravivaraṇaṁ,",
  "anya-e0701n1:11_14": "abhidhammatthasaṅgahassa ṭīkā vivaraṇaṁ,",
  "anya-e0701n1:11_15": "mahāvessantarājātakassa vivaraṇaṁ,",
  "anya-e0701n1:11_16": "sakkābhimataṁ,",
  "anya-e0701n1:11_17": "mahāvessantarajātakassa navaṭṭhakathā,",
  "anya-e0701n1:11_18": "paṭhama saṁbodhi,",
  "anya-e0701n1:11_19": "lokanetti ca,",
  "anya-e0701n1:11_20": "buddhaghosācariyanidānaṁ milindapañhā vaṇṇanā,",
  "anya-e0701n1:11_21": "caturā rakkhā,",
  "anya-e0701n1:11_22": "caturakkhāya aṭṭhakathā,",
  "anya-e0701n1:11_23": "saddavuttipakaraṇassa navaṭīkā,",
  "anya-e0701n1:11_24": "iccevaṁ pañcavīsati pamāṇāni pakaraṇāni laṅko dīpādīsuṭṭhānesu paṇḍitehi katāni ahesuṁ,",
  "anya-e0701n1:11_25": "sambuddhegāthā ca,",
  "anya-e0701n1:11_26": "naradeva nāma gāthā ca,",
  "anya-e0701n1:11_27": "dātave cīratti gāthā ca,",
  "anya-e0701n1:11_28": "vīsati ovādagāthā ca,",
  "anya-e0701n1:11_29": "dānasattari,",
  "anya-e0701n1:11_30": "sīlasattari,",
  "anya-e0701n1:11_31": "sappādānavaṇṇanā,",
  "anya-e0701n1:11_32": "anantabuddhavandanagāthā ca,",
  "anya-e0701n1:11_33": "aṭṭhavīsati buddhavandanagāthā ca,",
  "anya-e0701n1:11_34": "atītānāgatapaccuppannavandanagāthā ca,",
  "anya-e0701n1:11_35": "asītimahāsāvakavandanagāthā ca,",
  "anya-e0701n1:11_36": "navahāraguṇavaṇṇanā cāti,",
  "anya-e0701n1:11_37": "ime buddhapaṇāma-",
  "anya-e0701n1:11_38": "gāthādikā gāthā yo paṇḍitehi laṅkādīpādisuṭṭhānesu katā ahesuṁ [iti cullaganthavaṁse ganthakārakācariya dīpako nāma dutiyo paricchedo]"
}