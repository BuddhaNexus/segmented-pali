{
  "anya-e1005n0:0_0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e1005n0:1_0": "Lokanīti",
  "anya-e1005n0:2_0": "1. Paṇḍitakaṇḍo",
  "anya-e1005n0:3_0": "1.",
  "anya-e1005n0:4_0": "Lokanītiṃ pavakkhāmi,",
  "anya-e1005n0:5_0": "Nānāsatthasamuddhaṭaṃ;",
  "anya-e1005n0:6_0": "Māgadheneva saṅkhepaṃ,",
  "anya-e1005n0:7_0": "Vanditvā ratanatthayaṃ.",
  "anya-e1005n0:8_0": "2.",
  "anya-e1005n0:9_0": "Nītiloke purisassa sāro,",
  "anya-e1005n0:10_0": "Mātā pitā ācariyoca mitto;",
  "anya-e1005n0:11_0": "Tasmā hi nītiṃ puriso vijaññā,",
  "anya-e1005n0:12_0": "Ñāṇī mahā hoti bahussutoca.",
  "anya-e1005n0:13_0": "3.",
  "anya-e1005n0:14_0": "Alasassa kuto sippaṃ,",
  "anya-e1005n0:15_0": "Asippassa kuto dhanaṃ;",
  "anya-e1005n0:16_0": "Adhanassa kuto mittaṃ,",
  "anya-e1005n0:17_0": "Amittassa kuto sukhaṃ;",
  "anya-e1005n0:18_0": "Asukhassa kuto puññaṃ,",
  "anya-e1005n0:19_0": "Apuññassa kuto varaṃ.",
  "anya-e1005n0:20_0": "4.",
  "anya-e1005n0:21_0": "Sippā samaṃ dhanaṃ natthi,",
  "anya-e1005n0:22_0": "Sippaṃ corā nagaṇhare;",
  "anya-e1005n0:23_0": "Idha loke sippaṃ mittaṃ,",
  "anya-e1005n0:24_0": "Paraloke sukhāvahaṃ.",
  "anya-e1005n0:25_0": "5.",
  "anya-e1005n0:26_0": "Appakaṃ nātimaññeyya,",
  "anya-e1005n0:27_0": "Citte sutaṃ nidhāpaye;",
  "anya-e1005n0:28_0": "Vammikodakabindūva,",
  "anya-e1005n0:29_0": "Cirena paripūrati.",
  "anya-e1005n0:30_0": "6.",
  "anya-e1005n0:31_0": "Khuddoti nātimaññeyya,",
  "anya-e1005n0:32_0": "Vijjaṃ vā sippameva vā;",
  "anya-e1005n0:33_0": "Ekampi pariyodātaṃ,",
  "anya-e1005n0:34_0": "Jīvitakappakāraṇaṃ.",
  "anya-e1005n0:35_0": "7.",
  "anya-e1005n0:36_0": "Sele sele na māṇikaṃ,",
  "anya-e1005n0:37_0": "Gaje gaje na muttikaṃ;",
  "anya-e1005n0:38_0": "Vane vane na candanaṃ,",
  "anya-e1005n0:39_0": "Ṭhāne ṭhāne na paṇḍitaṃ.",
  "anya-e1005n0:40_0": "8.",
  "anya-e1005n0:41_0": "Paṇḍito sutasampanno,",
  "anya-e1005n0:42_0": "Yattha atthīti ce suto;",
  "anya-e1005n0:43_0": "Mahussāhena taṃ ṭhānaṃ,",
  "anya-e1005n0:44_0": "Gantabbaṃva sutesinā.",
  "anya-e1005n0:45_0": "9.",
  "anya-e1005n0:46_0": "Sine sippaṃ sine dhanaṃ,",
  "anya-e1005n0:47_0": "Sine pabbatamāruhaṃ;",
  "anya-e1005n0:48_0": "Sine kāmassa kodhassa,",
  "anya-e1005n0:49_0": "Ime pañca sine sine.",
  "anya-e1005n0:50_0": "10.",
  "anya-e1005n0:51_0": "Suti sammuti saṅkhyāca,",
  "anya-e1005n0:52_0": "Yogā nīti visesakā;",
  "anya-e1005n0:53_0": "Gandhabbā gaṇikā ceva,",
  "anya-e1005n0:54_0": "Dhanu bedā ca pūraṇā.",
  "anya-e1005n0:55_0": "11.",
  "anya-e1005n0:56_0": "Tikicchā itihāsā ca,",
  "anya-e1005n0:57_0": "Joti māyā ca chandati;",
  "anya-e1005n0:58_0": "Ketu mantā ca saddā ca,",
  "anya-e1005n0:59_0": "Sippāṭṭhārasakā ime.",
  "anya-e1005n0:60_0": "12.",
  "anya-e1005n0:61_0": "Apuṭṭho paṇḍito bherī,",
  "anya-e1005n0:62_0": "Pajjunno me hoti pucchito;",
  "anya-e1005n0:63_0": "Bālo puṭṭho apuṭṭhopi,",
  "anya-e1005n0:64_0": "Bahumpi bhaṇate sadā.",
  "anya-e1005n0:65_0": "13.",
  "anya-e1005n0:66_0": "Potthakesu ca yaṃ sippaṃ,",
  "anya-e1005n0:67_0": "Parahatthesu yaṃ dhanaṃ;",
  "anya-e1005n0:68_0": "Yathākicce samuppanne,",
  "anya-e1005n0:69_0": "Na taṃ sippaṃ na taṃ dhanaṃ.",
  "anya-e1005n0:70_0": "14.",
  "anya-e1005n0:71_0": "Jalappamāṇaṃ kumuddanālaṃ,",
  "anya-e1005n0:72_0": "Kulappamāṇaṃ vinayo pamāṇaṃ;",
  "anya-e1005n0:73_0": "Byattippamāṇaṃ kathītavākyaṃ,",
  "anya-e1005n0:74_0": "Pathaviyā pamāṇaṃ tiṇa milātaṃ.",
  "anya-e1005n0:75_0": "15.",
  "anya-e1005n0:76_0": "Appassuto sutaṃ appaṃ,",
  "anya-e1005n0:77_0": "Bahuṃ maññati mānavā;",
  "anya-e1005n0:78_0": "Sindhūdakaṃ apassanto,",
  "anya-e1005n0:79_0": "Kūpe toyaṃva maṇḍuko.",
  "anya-e1005n0:80_0": "16.",
  "anya-e1005n0:81_0": "Pathamaṃ parājaye sippaṃ,",
  "anya-e1005n0:82_0": "Dutiyaṃ parājaye dhanaṃ;",
  "anya-e1005n0:83_0": "Tatiyaṃ parājaye dhammaṃ,",
  "anya-e1005n0:84_0": "Catutthaṃ kiṃ karissati.",
  "anya-e1005n0:85_0": "17.",
  "anya-e1005n0:86_0": "Byatta putra kimalaso,",
  "anya-e1005n0:87_0": "Abyatto bhārahārako;",
  "anya-e1005n0:88_0": "Byattako pūjito loke,",
  "anya-e1005n0:89_0": "Byatta putra dine dine.",
  "anya-e1005n0:90_0": "18.",
  "anya-e1005n0:91_0": "Mātā verī pitā satru,",
  "anya-e1005n0:92_0": "Kena bāle na sikkhitā;",
  "anya-e1005n0:93_0": "Sabhāmajjhe na sobhanti,",
  "anya-e1005n0:94_0": "Haṃsamajjhe bakoyathā.",
  "anya-e1005n0:95_0": "19.",
  "anya-e1005n0:96_0": "Kaṇṭakaṃ giri ko tikkhati,",
  "anya-e1005n0:97_0": "Ko añjanaṃ migakkhikaṃ;",
  "anya-e1005n0:98_0": "Uppathaṃ pallale ko sugandhaṃ,",
  "anya-e1005n0:99_0": "Kula-putta-rūpo ko pavattati;",
  "anya-e1005n0:100_0": "Sāmaṃ-bhāvo.",
  "anya-e1005n0:101_0": "20.",
  "anya-e1005n0:102_0": "Na rasaṃ akotambulaṃ,",
  "anya-e1005n0:103_0": "Adhanassa, laṅkatampi;",
  "anya-e1005n0:104_0": "Alonakantu byañjanaṃ,",
  "anya-e1005n0:105_0": "Byākaraṇaṃ asippassa.",
  "anya-e1005n0:106_0": "21.",
  "anya-e1005n0:107_0": "Sussusā sutasampanno,",
  "anya-e1005n0:108_0": "Sutāpaññāya pavaḍḍhati;",
  "anya-e1005n0:109_0": "Paññāya atthaṃ jānāti,",
  "anya-e1005n0:110_0": "Ñāto attho sukhāvaho.",
  "anya-e1005n0:111_0": "22.",
  "anya-e1005n0:112_0": "Bhojanaṃ methunaṃ niddā,",
  "anya-e1005n0:113_0": "Goṇe posepi vijjati;",
  "anya-e1005n0:114_0": "Vijjā viseso posassa,",
  "anya-e1005n0:115_0": "Taṃ hīno gosamo bhave.",
  "anya-e1005n0:116_0": "23.",
  "anya-e1005n0:117_0": "Natthi vijjāsamaṃmittaṃ,",
  "anya-e1005n0:118_0": "Naca byādhisamo ripu;",
  "anya-e1005n0:119_0": "Naca attasamaṃ pemaṃ,",
  "anya-e1005n0:120_0": "Naca kammasamaṃ balaṃ.",
  "anya-e1005n0:121_0": "24.",
  "anya-e1005n0:122_0": "Haṃso majjhe na kākānaṃ,",
  "anya-e1005n0:123_0": "Sīho gunnaṃ na sobhate;",
  "anya-e1005n0:124_0": "Gadrabhamajjhe turaṅgo,",
  "anya-e1005n0:125_0": "Bālamajjhe ca paṇḍito.",
  "anya-e1005n0:126_0": "25.",
  "anya-e1005n0:127_0": "Yāvajīvampi ce bālo,",
  "anya-e1005n0:128_0": "Paṇḍitaṃ payirupāsati;",
  "anya-e1005n0:129_0": "Na so dhammaṃ vijānāti,",
  "anya-e1005n0:130_0": "Dabbi sūparasaṃ yathā.",
  "anya-e1005n0:131_0": "26.",
  "anya-e1005n0:132_0": "Muhuttamapi ce viññū,",
  "anya-e1005n0:133_0": "Paṇḍitaṃ payirupāsati;",
  "anya-e1005n0:134_0": "Khippaṃ dhammaṃ vijānāti,",
  "anya-e1005n0:135_0": "Jivhā sūparasaṃ yathā.",
  "anya-e1005n0:136_0": "27.",
  "anya-e1005n0:137_0": "Vinā satthaṃ na gaccheyya,",
  "anya-e1005n0:138_0": "Sūro saṅgāmabhūmiyaṃ;",
  "anya-e1005n0:139_0": "Paṇḍitvāddhagū vāṇijo,",
  "anya-e1005n0:140_0": "Videsagamano tathā.",
  "anya-e1005n0:141_0": "28.",
  "anya-e1005n0:142_0": "Dhananāsaṃ manotāpaṃ,",
  "anya-e1005n0:143_0": "Ghare duccaritāni ca;",
  "anya-e1005n0:144_0": "Vañcanañca avamānaṃ,",
  "anya-e1005n0:145_0": "Paṇḍito na pakāsaye.",
  "anya-e1005n0:146_0": "29.",
  "anya-e1005n0:147_0": "Pattānurūpakaṃ vākyaṃ,",
  "anya-e1005n0:148_0": "Sabhāvarūpakaṃ piyaṃ;",
  "anya-e1005n0:149_0": "Attānurūpakaṃ kodhaṃ,",
  "anya-e1005n0:150_0": "Yo jānāti sa paṇḍito.",
  "anya-e1005n0:151_0": "30.",
  "anya-e1005n0:152_0": "A-dhanassa rasaṃ khādā,",
  "anya-e1005n0:153_0": "A-balassa hathā narā;",
  "anya-e1005n0:154_0": "A-paññassa vākya-kathā,",
  "anya-e1005n0:155_0": "Ummattaka-samā ime.",
  "anya-e1005n0:156_0": "31.",
  "anya-e1005n0:157_0": "Anavhāyaṃ gamayanto,",
  "anya-e1005n0:158_0": "A-pucchā bahu-bhāsako;",
  "anya-e1005n0:159_0": "Atta-guṇaṃ pakāsento,",
  "anya-e1005n0:160_0": "Ti-vidhaṃ hīna-lakkhaṇaṃ.",
  "anya-e1005n0:161_0": "32.",
  "anya-e1005n0:162_0": "Appa-rūpo bahuṃ bhāso,",
  "anya-e1005n0:163_0": "Appa-pañño pakāsito;",
  "anya-e1005n0:164_0": "Appa-pūro ghaṭo khobhe,",
  "anya-e1005n0:165_0": "Appa-khīrā gāvī cathe.",
  "anya-e1005n0:166_0": "33.",
  "anya-e1005n0:167_0": "Maṇḍūkepi ukre sīhe,",
  "anya-e1005n0:168_0": "Kākaggahe piye piye;",
  "anya-e1005n0:169_0": "A-paṇḍīpi paṇḍī hutvā,",
  "anya-e1005n0:170_0": "Dhīrā pucche vaye vaye.",
  "anya-e1005n0:171_0": "34.",
  "anya-e1005n0:172_0": "Maṇḍūkepi ukre sīhe,",
  "anya-e1005n0:173_0": "Sūkarepi uhe dīpe;",
  "anya-e1005n0:174_0": "Biḷāre sadise byagghe,",
  "anya-e1005n0:175_0": "Sabba dhīre sippa-same.",
  "anya-e1005n0:176_0": "35.",
  "anya-e1005n0:177_0": "Na titti rājā dhanamhi,",
  "anya-e1005n0:178_0": "Paṇḍitopi su-bhāsite;",
  "anya-e1005n0:179_0": "Cakkhuṃpi piya-dassane,",
  "anya-e1005n0:180_0": "Jale sāgaro na titti.",
  "anya-e1005n0:181_0": "36.",
  "anya-e1005n0:182_0": "Rūpa-yobbana-sampannā,",
  "anya-e1005n0:183_0": "Visātha-kutha-sambhavā;",
  "anya-e1005n0:184_0": "Vijjā-hīnā na sobhanti,",
  "anya-e1005n0:185_0": "Nigandhā iva kiṃsukā.",
  "anya-e1005n0:186_0": "37.",
  "anya-e1005n0:187_0": "Hīne putto rājāmacco,",
  "anya-e1005n0:188_0": "Bāla-putto ca paṇḍito;",
  "anya-e1005n0:189_0": "A-dhanassa dhanaṃ bahu,",
  "anya-e1005n0:190_0": "Purisānaṃ na maññatha.",
  "anya-e1005n0:191_0": "38.",
  "anya-e1005n0:192_0": "Yo sippa-lobhena,",
  "anya-e1005n0:193_0": "Bahuṃ gaṇhāti taṃ sippaṃ;",
  "anya-e1005n0:194_0": "Mūgova supinaṃ passaṃ,",
  "anya-e1005n0:195_0": "Kathetumpi na ussahe.",
  "anya-e1005n0:196_0": "39.",
  "anya-e1005n0:197_0": "Bhijjetuṃ kumbhakāro,",
  "anya-e1005n0:198_0": "Sobhetuṃ kumbha ghaṭṭati;",
  "anya-e1005n0:199_0": "Na khipituṃ apāyesu,",
  "anya-e1005n0:200_0": "Sissānaṃ vuḍḍhi-kāraṇā.",
  "anya-e1005n0:201_0": "40.",
  "anya-e1005n0:202_0": "Taggarañca palāsena,",
  "anya-e1005n0:203_0": "Yo naro upanayhati;",
  "anya-e1005n0:204_0": "Pattāpi surabhi vāyanti,",
  "anya-e1005n0:205_0": "Evaṃ dhīrūpasevanā.",
  "anya-e1005n0:206_0": "Paṇḍitakaṇḍo niṭṭhito.",
  "anya-e1005n0:207_0": "Sujanakaṇḍo",
  "anya-e1005n0:208_0": "41.",
  "anya-e1005n0:209_0": "Sabbhireva samāsetha,",
  "anya-e1005n0:210_0": "Sabbhi kubbetha santhavaṃ;",
  "anya-e1005n0:211_0": "Sataṃ saddhammamaññāya,",
  "anya-e1005n0:212_0": "Seyyo hoti na pāpiyo.",
  "anya-e1005n0:213_0": "42.",
  "anya-e1005n0:214_0": "Caja dujjana saṃsaggaṃ,",
  "anya-e1005n0:215_0": "Bhaja sādhu samāgamaṃ;",
  "anya-e1005n0:216_0": "Kara puññamahorattiṃ,",
  "anya-e1005n0:217_0": "Sara niccamaniccataṃ.",
  "anya-e1005n0:218_0": "43.",
  "anya-e1005n0:219_0": "Yathā udumbarapakkā,",
  "anya-e1005n0:220_0": "Bahirattakamevaca;",
  "anya-e1005n0:221_0": "Anto kimīhi sampuṇṇā,",
  "anya-e1005n0:222_0": "Evaṃ dujjanahaddayā.",
  "anya-e1005n0:223_0": "44.",
  "anya-e1005n0:224_0": "Yathāpi panasāpakkā,",
  "anya-e1005n0:225_0": "Bahi kaṇḍakameva ca;",
  "anya-e1005n0:226_0": "Anto amatasampannā,",
  "anya-e1005n0:227_0": "Evaṃ sujanahadayā.",
  "anya-e1005n0:228_0": "45.",
  "anya-e1005n0:229_0": "Sukkhopi candanataru na jahāti gandhaṃ,",
  "anya-e1005n0:230_0": "Nāgo gato naramukhe na jahāti līḷaṃ;",
  "anya-e1005n0:231_0": "Yantāgato madhurasaṃ na jahāti ucchu,",
  "anya-e1005n0:232_0": "Dukkhopi paṇḍitajano na jahāti dhammaṃ.",
  "anya-e1005n0:233_0": "46.",
  "anya-e1005n0:234_0": "Sīho nāma jighacchāpi,",
  "anya-e1005n0:235_0": "Paṇṇādīni na khādati;",
  "anya-e1005n0:236_0": "Sīho nāma kisocāpi,",
  "anya-e1005n0:237_0": "Nāgamaṃsaṃ na khādati.",
  "anya-e1005n0:238_0": "47.",
  "anya-e1005n0:239_0": "Kula-jāto kula-putto,",
  "anya-e1005n0:240_0": "Kula-vaṃsa-surakkhato;",
  "anya-e1005n0:241_0": "Attanā dukkha-pattopi,",
  "anya-e1005n0:242_0": "Hīna-kammaṃ na kāraye.",
  "anya-e1005n0:243_0": "48.",
  "anya-e1005n0:244_0": "Candanaṃ sītalaṃ loke,",
  "anya-e1005n0:245_0": "Tato candaṃva sītalaṃ;",
  "anya-e1005n0:246_0": "Canda-candanasītamhā,",
  "anya-e1005n0:247_0": "Sādhu vākyaṃ subhāsitaṃ.",
  "anya-e1005n0:248_0": "49.",
  "anya-e1005n0:249_0": "Udeyya bhāṇu pacchime,",
  "anya-e1005n0:250_0": "Merurājā nameyyapi;",
  "anya-e1005n0:251_0": "Sītalā narakaggipi,",
  "anya-e1005n0:252_0": "Pabbatagge ca uppalaṃ.",
  "anya-e1005n0:253_0": "Vikase na viparītaṃ,",
  "anya-e1005n0:254_0": "Sādhuvāyyaṃ kudācanaṃ.",
  "anya-e1005n0:255_0": "50.",
  "anya-e1005n0:256_0": "Sukhā rukkhassa chāyāva,",
  "anya-e1005n0:257_0": "Tato ñāti mātā pitu;",
  "anya-e1005n0:258_0": "Tato ācariyo rañño,",
  "anya-e1005n0:259_0": "Tato buddhassanekadhā.",
  "anya-e1005n0:260_0": "51.",
  "anya-e1005n0:261_0": "Bhamarā pupphamicchanti,",
  "anya-e1005n0:262_0": "Guṇamicchanti sujanā;",
  "anya-e1005n0:263_0": "Makkhikā pūtimicchanti,",
  "anya-e1005n0:264_0": "Dosamicchanti dujjanā.",
  "anya-e1005n0:265_0": "52.",
  "anya-e1005n0:266_0": "Mātāhīnassa dubbhāsā,",
  "anya-e1005n0:267_0": "Pitāhīnassa dukriyā;",
  "anya-e1005n0:268_0": "Ubho mātā pitā hīnā,",
  "anya-e1005n0:269_0": "Dubbhasāca dukīriyā.",
  "anya-e1005n0:270_0": "53.",
  "anya-e1005n0:271_0": "Mātā seṭṭhassa subhāsā,",
  "anya-e1005n0:272_0": "Pitā seṭṭhassa sukriyā;",
  "anya-e1005n0:273_0": "Ubho mātā pitā seṭṭhā,",
  "anya-e1005n0:274_0": "Subhāsāca sukīriyā.",
  "anya-e1005n0:275_0": "54.",
  "anya-e1005n0:276_0": "Saṅgāme sūra-micchanti,",
  "anya-e1005n0:277_0": "Mantīsu akūtūhalaṃ;",
  "anya-e1005n0:278_0": "Piyañca annapānesu,",
  "anya-e1005n0:279_0": "Atthe jāte ca paṇḍitaṃ.",
  "anya-e1005n0:280_0": "55.",
  "anya-e1005n0:281_0": "Sunakho sunakhaṃ disvā,",
  "anya-e1005n0:282_0": "Dantaṃ dasseti hiṃsituṃ;",
  "anya-e1005n0:283_0": "Dujjano sujanaṃ disvā,",
  "anya-e1005n0:284_0": "Rosayaṃ hiṃsamicchati.",
  "anya-e1005n0:285_0": "56.",
  "anya-e1005n0:286_0": "Mā ca vegena kiccāni,",
  "anya-e1005n0:287_0": "Karosi kārāpesi vā;",
  "anya-e1005n0:288_0": "Sahasā kāritaṃ kammaṃ,",
  "anya-e1005n0:289_0": "Mando pacchānutappati.",
  "anya-e1005n0:290_0": "57.",
  "anya-e1005n0:291_0": "Kodhaṃ vihitvā na kadācinasoce,",
  "anya-e1005n0:292_0": "Makkhappahānaṃ isayo avaṇṇayuṃ;",
  "anya-e1005n0:293_0": "Sabbesa phārusa-vacaṃ khametha,",
  "anya-e1005n0:294_0": "Etaṃ khantiṃ uttamamāhu santo.",
  "anya-e1005n0:295_0": "58.",
  "anya-e1005n0:296_0": "Dukkho nivāso sambādhe,",
  "anya-e1005n0:297_0": "Ṭhāne asucisaṅkate;",
  "anya-e1005n0:298_0": "Tato arimhi appiye,",
  "anya-e1005n0:299_0": "Tatopi akataññunā.",
  "anya-e1005n0:300_0": "59.",
  "anya-e1005n0:301_0": "Ovādeyyānusāseyya,",
  "anya-e1005n0:302_0": "Gāpakā ca nivāraye;",
  "anya-e1005n0:303_0": "Satañhi so piyo hoti,",
  "anya-e1005n0:304_0": "Asataṃ hoti appiyo.",
  "anya-e1005n0:305_0": "60.",
  "anya-e1005n0:306_0": "Uttamattanivātena,",
  "anya-e1005n0:307_0": "Sūraṃ bhedena nijjaye;",
  "anya-e1005n0:308_0": "Nīcaṃ appaka dānena,",
  "anya-e1005n0:309_0": "Vīriyena samaṃ jaye.",
  "anya-e1005n0:310_0": "61.",
  "anya-e1005n0:311_0": "Na visaṃ visamiccāhu,",
  "anya-e1005n0:312_0": "Dhanaṃ saṅghassa uccate;",
  "anya-e1005n0:313_0": "Visaṃ ekaṃva hanati,",
  "anya-e1005n0:314_0": "Sabbaṃ saṅghassa santakaṃ.",
  "anya-e1005n0:315_0": "62.",
  "anya-e1005n0:316_0": "Javane bhadraṃ jānanti,",
  "anya-e1005n0:317_0": "Baliddañca vāhenā;",
  "anya-e1005n0:318_0": "Duhena dhenuṃ jānanti,",
  "anya-e1005n0:319_0": "Bhāsamānena paṇḍitaṃ.",
  "anya-e1005n0:320_0": "63.",
  "anya-e1005n0:321_0": "Dhanamappampi sādhūnaṃ,",
  "anya-e1005n0:322_0": "Kūpe vāriva nissayo;",
  "anya-e1005n0:323_0": "Bahuṃ api asādhūnaṃ,",
  "anya-e1005n0:324_0": "Naca vārīva aṇṇave.",
  "anya-e1005n0:325_0": "64.",
  "anya-e1005n0:326_0": "Najjo pivanti no āpaṃ,",
  "anya-e1005n0:327_0": "Rukkhā khādanti no phalaṃ;",
  "anya-e1005n0:328_0": "Vassanti kvaci no meghā,",
  "anya-e1005n0:329_0": "Paratthāya sataṃ dhanaṃ.",
  "anya-e1005n0:330_0": "65.",
  "anya-e1005n0:331_0": "Apattheyyaṃ na pattheyya,",
  "anya-e1005n0:332_0": "A cinteyyaṃ na cintaye;",
  "anya-e1005n0:333_0": "Dhammameva sucinteyya,",
  "anya-e1005n0:334_0": "Kālaṃ moghaṃ na accaye.",
  "anya-e1005n0:335_0": "66.",
  "anya-e1005n0:336_0": "Acintitampi bhavati,",
  "anya-e1005n0:337_0": "Cintitampi vinassati;",
  "anya-e1005n0:338_0": "Na hi cintāmayā bhogā,",
  "anya-e1005n0:339_0": "Itthiyā purisassavā.",
  "anya-e1005n0:340_0": "67.",
  "anya-e1005n0:341_0": "Asantassa piyo hoti,",
  "anya-e1005n0:342_0": "Sante na kurute piyaṃ;",
  "anya-e1005n0:343_0": "Asataṃ dhammaṃ roceti,",
  "anya-e1005n0:344_0": "Taṃ parābhavato mukhaṃ.",
  "anya-e1005n0:345_0": "Sujanakaṇḍo niṭṭhito.",
  "anya-e1005n0:346_0": "4. Bāladujjanakaṇḍo",
  "anya-e1005n0:347_0": "68.",
  "anya-e1005n0:348_0": "Atippiyo na kātabbo,",
  "anya-e1005n0:349_0": "Khalo kotūhalaṃ karo;",
  "anya-e1005n0:350_0": "Sirasā vahyamānopi,",
  "anya-e1005n0:351_0": "Aḍḍhapūro ghaṭo yathā.",
  "anya-e1005n0:352_0": "69.",
  "anya-e1005n0:353_0": "Sappo duṭṭho khalo duṭṭho,",
  "anya-e1005n0:354_0": "Sappo duṭṭhataro khalo;",
  "anya-e1005n0:355_0": "Mantosadhehi so sappo,",
  "anya-e1005n0:356_0": "Khalo kenupasammati.",
  "anya-e1005n0:357_0": "70.",
  "anya-e1005n0:358_0": "Yo bālo maññati bālyaṃ,",
  "anya-e1005n0:359_0": "Paṇḍito vāpi tena so;",
  "anya-e1005n0:360_0": "Bālova paṇḍitamānī,",
  "anya-e1005n0:361_0": "So ve bāloti vuccati.",
  "anya-e1005n0:362_0": "71.",
  "anya-e1005n0:363_0": "Madhuṃva maññatī bālo,",
  "anya-e1005n0:364_0": "Yāva pāpaṃ na paccati;",
  "anya-e1005n0:365_0": "Yadāca paccatī pāpaṃ,",
  "anya-e1005n0:366_0": "Atha dukkhaṃ nigacchati.",
  "anya-e1005n0:367_0": "72.",
  "anya-e1005n0:368_0": "Na sādhu balavā bālo,",
  "anya-e1005n0:369_0": "Sahasā vindate dhanaṃ;",
  "anya-e1005n0:370_0": "Kāyassa bhedā duppañño,",
  "anya-e1005n0:371_0": "Nirayaṃ sopapajjati.",
  "anya-e1005n0:372_0": "73.",
  "anya-e1005n0:373_0": "Ghare duṭṭho ca mūsīko ca,",
  "anya-e1005n0:374_0": "Vane duṭṭho ca vānaro;",
  "anya-e1005n0:375_0": "Sakuṇe ca duṭṭho kāko,",
  "anya-e1005n0:376_0": "Nare duṭṭhoca brāhmaṇo.",
  "anya-e1005n0:377_0": "74.",
  "anya-e1005n0:378_0": "Dīghā jāgarato ratti,",
  "anya-e1005n0:379_0": "Dīghaṃ santassa yojanaṃ;",
  "anya-e1005n0:380_0": "Dīgho bālāna saṃsāro,",
  "anya-e1005n0:381_0": "Saddhammaṃ a-vijānataṃ.",
  "anya-e1005n0:382_0": "75.",
  "anya-e1005n0:383_0": "Tila mattaṃ paresaṃva,",
  "anya-e1005n0:384_0": "Appa dosañca passati;",
  "anya-e1005n0:385_0": "Nāḷikerampi sadosaṃ,",
  "anya-e1005n0:386_0": "Khala-jāto na passati.",
  "anya-e1005n0:387_0": "76.",
  "anya-e1005n0:388_0": "Nattadosaṃ pare jaññā,",
  "anya-e1005n0:389_0": "Jaññā dosaṃ parassatu;",
  "anya-e1005n0:390_0": "Guyhe kummo aṅgāni,",
  "anya-e1005n0:391_0": "Para dosañca lakkhaye.",
  "anya-e1005n0:392_0": "77.",
  "anya-e1005n0:393_0": "Paṇḍitassa pasaṃsāya,",
  "anya-e1005n0:394_0": "Daṇḍo bālena dīyate;",
  "anya-e1005n0:395_0": "Paṇḍito paṇḍiteneva,",
  "anya-e1005n0:396_0": "Vaṇṇitova suvaṇṇito.",
  "anya-e1005n0:397_0": "78.",
  "anya-e1005n0:398_0": "Luddhaṃ atthena gaṇheyya,",
  "anya-e1005n0:399_0": "Thaddhaṃ añjali kammunā;",
  "anya-e1005n0:400_0": "Chandānuvattiyā mūḷhaṃ,",
  "anya-e1005n0:401_0": "Yathābhūtena paṇḍitaṃ.",
  "anya-e1005n0:402_0": "Bāladujjanakaṇḍo niṭṭhito.",
  "anya-e1005n0:403_0": "4. Mittakaṇḍo",
  "anya-e1005n0:404_0": "79.",
  "anya-e1005n0:405_0": "Hitakāro paro bandhu,",
  "anya-e1005n0:406_0": "Bandhūpi ahito paro;",
  "anya-e1005n0:407_0": "A hito dehajo byādhi,",
  "anya-e1005n0:408_0": "Hitaṃ araññamosadhaṃ.",
  "anya-e1005n0:409_0": "80.",
  "anya-e1005n0:410_0": "Parokkhe guṇa-hantāraṃ,",
  "anya-e1005n0:411_0": "Paccakkhe piyavādinaṃ;",
  "anya-e1005n0:412_0": "Vajjeyya tādisaṃ mittaṃ,",
  "anya-e1005n0:413_0": "Visakumbhe yathā madhuṃ.",
  "anya-e1005n0:414_0": "81.",
  "anya-e1005n0:415_0": "Dhanahīne caje mitto,",
  "anya-e1005n0:416_0": "Puttadārā sahodarā;",
  "anya-e1005n0:417_0": "Dhanavantaṃva sevanti,",
  "anya-e1005n0:418_0": "Dhanaṃ loke mahā sakhā.",
  "anya-e1005n0:419_0": "82.",
  "anya-e1005n0:420_0": "Jāneyya pesena bhaccaṃ,",
  "anya-e1005n0:421_0": "Bandhuṃ vāpi bhayāgate;",
  "anya-e1005n0:422_0": "Appakāsu tathā mittaṃ,",
  "anya-e1005n0:423_0": "Dārañca vibhavakkhaye.",
  "anya-e1005n0:424_0": "83.",
  "anya-e1005n0:425_0": "So bandhu yo hite yutto,",
  "anya-e1005n0:426_0": "Pitaro honti posako;",
  "anya-e1005n0:427_0": "Taṃ mittaṃ yattha vissāso,",
  "anya-e1005n0:428_0": "Sā bhariyā ca yassa nibbūti.",
  "anya-e1005n0:429_0": "84.",
  "anya-e1005n0:430_0": "Na vissase a-vissattaṃ,",
  "anya-e1005n0:431_0": "Mittañcāpi na vissase;",
  "anya-e1005n0:432_0": "Kadāci kupito mito,",
  "anya-e1005n0:433_0": "Sabbaṃ dosaṃ pakāsaye.",
  "anya-e1005n0:434_0": "85.",
  "anya-e1005n0:435_0": "Sakiṃ duṭṭhañca yo mittaṃ,",
  "anya-e1005n0:436_0": "Puna sandhitumicchati;",
  "anya-e1005n0:437_0": "So maccuṃ upagaṇhāti,",
  "anya-e1005n0:438_0": "Gabbhamassatarī yathā.",
  "anya-e1005n0:439_0": "86.",
  "anya-e1005n0:440_0": "Vahe a-mittaṃ khandhena,",
  "anya-e1005n0:441_0": "Yāva kālo anāgato;",
  "anya-e1005n0:442_0": "Tameva āgate kāle,",
  "anya-e1005n0:443_0": "Sele bhinde ghaṭaṃ iva.",
  "anya-e1005n0:444_0": "87.",
  "anya-e1005n0:445_0": "Iṇaseso aggiseso,",
  "anya-e1005n0:446_0": "Satruseso tatheva ca;",
  "anya-e1005n0:447_0": "Punappunaṃ vivaḍḍhanti,",
  "anya-e1005n0:448_0": "Tasmā sesaṃ na kāraye.",
  "anya-e1005n0:449_0": "88.",
  "anya-e1005n0:450_0": "Padumaṃva mukhaṃ yassa,",
  "anya-e1005n0:451_0": "Vācā candana sītalā;",
  "anya-e1005n0:452_0": "Tādisaṃ nopaseveyya,",
  "anya-e1005n0:453_0": "Hadayetu halāhalaṃ.",
  "anya-e1005n0:454_0": "89.",
  "anya-e1005n0:455_0": "Na seve pharusaṃ sāmiṃ,",
  "anya-e1005n0:456_0": "Naca seveyya macchariṃ;",
  "anya-e1005n0:457_0": "Tato apaggaṇhaṃ sāmiṃ,",
  "anya-e1005n0:458_0": "Neva niggahitaṃ tato.",
  "anya-e1005n0:459_0": "90.",
  "anya-e1005n0:460_0": "Siṅgī paññāsa hatthena,",
  "anya-e1005n0:461_0": "Vajje satena vājinaṃ;",
  "anya-e1005n0:462_0": "Hatthiṃ dantiṃ sahassena,",
  "anya-e1005n0:463_0": "Desa cāgena dujjanaṃ.",
  "anya-e1005n0:464_0": "91.",
  "anya-e1005n0:465_0": "Kudesañca kumittañca,",
  "anya-e1005n0:466_0": "Kukulañca kubandhavaṃ;",
  "anya-e1005n0:467_0": "Kudārañca kudāsañca,",
  "anya-e1005n0:468_0": "Dūrato parivajjaye.",
  "anya-e1005n0:469_0": "92.",
  "anya-e1005n0:470_0": "Rogāture ca dubbhikkhe,",
  "anya-e1005n0:471_0": "Byasane sattu viggahe;",
  "anya-e1005n0:472_0": "Rājadvāre susāne ca,",
  "anya-e1005n0:473_0": "Ye tiṭṭhanti sumittakā.",
  "anya-e1005n0:474_0": "93.",
  "anya-e1005n0:475_0": "Sītavāco bahumitto,",
  "anya-e1005n0:476_0": "Pharuso appamittako;",
  "anya-e1005n0:477_0": "Upamaṃ ettha ñātabbā,",
  "anya-e1005n0:478_0": "Canda-sūriya-rājūnaṃ.",
  "anya-e1005n0:479_0": "Mittakaṇḍo niṭṭhito.",
  "anya-e1005n0:480_0": "5. Itthikaṇḍo",
  "anya-e1005n0:481_0": "94.",
  "anya-e1005n0:482_0": "Kokilānaṃ saddaṃ rūpaṃ,",
  "anya-e1005n0:483_0": "Nārīrūpaṃ patibbatā;",
  "anya-e1005n0:484_0": "Vijjā rūpaṃ a-rūpānaṃ,",
  "anya-e1005n0:485_0": "Khamā rūpaṃ tapassinaṃ.",
  "anya-e1005n0:486_0": "95.",
  "anya-e1005n0:487_0": "Itthīnañca dhanaṃ rūpaṃ,",
  "anya-e1005n0:488_0": "Purisānaṃ vijjā dhanaṃ;",
  "anya-e1005n0:489_0": "Bhikkhūnañca dhanaṃ sīlaṃ,",
  "anya-e1005n0:490_0": "Rājānañca dhanaṃ balaṃ.",
  "anya-e1005n0:491_0": "96.",
  "anya-e1005n0:492_0": "Tapassino kisā sobhā,",
  "anya-e1005n0:493_0": "Thūlā sobhā catuppadā;",
  "anya-e1005n0:494_0": "Purisā vijjavā sobhā,",
  "anya-e1005n0:495_0": "Itthī sobhāsa sāmikā.",
  "anya-e1005n0:496_0": "97.",
  "anya-e1005n0:497_0": "Pañca ratyā sugandhabbā,",
  "anya-e1005n0:498_0": "Satta ratyā dhanuggahā;",
  "anya-e1005n0:499_0": "Eka māsā subhariyā,",
  "anya-e1005n0:500_0": "Aḍḍha māsā sissā malā.",
  "anya-e1005n0:501_0": "98.",
  "anya-e1005n0:502_0": "Hiṃ ramati paṅa,",
  "anya-e1005n0:503_0": "Haṅa ramati poka.",
  "anya-e1005n0:504_0": "Thī ramati pu,",
  "anya-e1005n0:505_0": "Khu ramati dhaṃ.",
  "anya-e1005n0:506_0": "99.",
  "anya-e1005n0:507_0": "Jiṇṇamannaṃ pasaṃseyya,",
  "anya-e1005n0:508_0": "Dārañca gatayobbanaṃ;",
  "anya-e1005n0:509_0": "Raṇā punāgatā sūraṃ,",
  "anya-e1005n0:510_0": "Sassañca gehamāgataṃ.",
  "anya-e1005n0:511_0": "100.",
  "anya-e1005n0:512_0": "Dvatti-patikā nārī ca,",
  "anya-e1005n0:513_0": "Bhikkhu dvatti-vihāriko;",
  "anya-e1005n0:514_0": "Dvatti-pāsa-mutto pakkhī,",
  "anya-e1005n0:515_0": "Kata-māyā bahūtharaṃ.",
  "anya-e1005n0:516_0": "101.",
  "anya-e1005n0:517_0": "Dujjanaṃ pahārādame,",
  "anya-e1005n0:518_0": "Mittaṃ dame a-bhāṇikā;",
  "anya-e1005n0:519_0": "Itthiñca byasanā dame,",
  "anya-e1005n0:520_0": "Rāginaṃ appa bhojanā.",
  "anya-e1005n0:521_0": "102.",
  "anya-e1005n0:522_0": "Na ratti vinā candimā,",
  "anya-e1005n0:523_0": "Vīciṃ vinā ca aṇṇavo;",
  "anya-e1005n0:524_0": "Haṃsaṃ vinā pokkharaṇī,",
  "anya-e1005n0:525_0": "Patiṃ kaññāca sobhate.",
  "anya-e1005n0:526_0": "103.",
  "anya-e1005n0:527_0": "Patinā janito bhogo,",
  "anya-e1005n0:528_0": "Itthiyāva saṃgoppito;",
  "anya-e1005n0:529_0": "Purisova hi padhāno,",
  "anya-e1005n0:530_0": "Itthī suttaṃva sūciyā.",
  "anya-e1005n0:531_0": "104.",
  "anya-e1005n0:532_0": "Sabbānadī vaṅkanadī,",
  "anya-e1005n0:533_0": "Sabbe kaṭṭhamayā vanā;",
  "anya-e1005n0:534_0": "Sabbitthiyo kare pāpaṃ,",
  "anya-e1005n0:535_0": "Labhamāne nivātake.",
  "anya-e1005n0:536_0": "105.",
  "anya-e1005n0:537_0": "Vivādasīliṃ usūyabhāṇiniṃ,",
  "anya-e1005n0:538_0": "Sampassataṇhiṃ bahupākabhuttiniṃ;",
  "anya-e1005n0:539_0": "Aggantabhuttiṃ paragehavāsiniṃ,",
  "anya-e1005n0:540_0": "Nāriṃ caje puttasatampi pūmā.",
  "anya-e1005n0:541_0": "106.",
  "anya-e1005n0:542_0": "Bhuttesu maṇḍesu janīva kantinī,",
  "anya-e1005n0:543_0": "Guyheca ṭhāne bhaginīva hiriṇī;",
  "anya-e1005n0:544_0": "Kammesu pattesu karoti dāsīva,",
  "anya-e1005n0:545_0": "Bhayesu mantī sayanesu rāmaye;",
  "anya-e1005n0:546_0": "Rūpīsu sikkhī kupanesu khantinī,",
  "anya-e1005n0:547_0": "Sā nārī seṭṭhāti vadanti paṇḍitā;",
  "anya-e1005n0:548_0": "Kāyassa bhedāca divebhaveyya sā.",
  "anya-e1005n0:549_0": "107.",
  "anya-e1005n0:550_0": "Sāmā migakkhī tanumajjhagattā,",
  "anya-e1005n0:551_0": "Sūrū sukesī samadantapantī;",
  "anya-e1005n0:552_0": "Gambhīranābhī yuvatī susīlī,",
  "anya-e1005n0:553_0": "Hīne kule jātāpi vivāhyā.",
  "anya-e1005n0:554_0": "108.",
  "anya-e1005n0:555_0": "Saradaṃratu-kālānaṃ,",
  "anya-e1005n0:556_0": "Bhariyānaṃ rūpavatī;",
  "anya-e1005n0:557_0": "Jeṭṭho padhānaṃ puttānaṃ,",
  "anya-e1005n0:558_0": "Disānaṃ uttarādisā.",
  "anya-e1005n0:559_0": "109.",
  "anya-e1005n0:560_0": "Yā icche puriso hotuṃ,",
  "anya-e1005n0:561_0": "Jāti jāti punappunaṃ;",
  "anya-e1005n0:562_0": "Sāmikaṃ apacāyeyya,",
  "anya-e1005n0:563_0": "Indaṃva pāricārikā.",
  "anya-e1005n0:564_0": "110.",
  "anya-e1005n0:565_0": "Yo icche puriso hotuṃ,",
  "anya-e1005n0:566_0": "Jāti jāti punappunaṃ;",
  "anya-e1005n0:567_0": "Paradānaṃ vivajjeyya,",
  "anya-e1005n0:568_0": "Dhotapādova kaddamaṃ.",
  "anya-e1005n0:569_0": "111.",
  "anya-e1005n0:570_0": "Atikkanta vayo poso,",
  "anya-e1005n0:571_0": "Āneti timbaruttaniṃ;",
  "anya-e1005n0:572_0": "Tassā issā asaddhāti,",
  "anya-e1005n0:573_0": "Taṃ parābhavato mukhaṃ.",
  "anya-e1005n0:574_0": "Itthikaṇḍo niṭṭhito.",
  "anya-e1005n0:575_0": "6. Rājakaṇḍo",
  "anya-e1005n0:576_0": "112.",
  "anya-e1005n0:577_0": "Ekayāmaṃ saye rājā,",
  "anya-e1005n0:578_0": "Dviyāmaññeva paṇḍito;",
  "anya-e1005n0:579_0": "Gharāvāso tiyāmaṃva,",
  "anya-e1005n0:580_0": "Catuyāmaṃ tu yācako.",
  "anya-e1005n0:581_0": "113.",
  "anya-e1005n0:582_0": "Dhanavā sutavā rājā,",
  "anya-e1005n0:583_0": "Nadī vejjo cimepañca;",
  "anya-e1005n0:584_0": "Yattha dese na vijjanti,",
  "anya-e1005n0:585_0": "Na tattha divasaṃ vase.",
  "anya-e1005n0:586_0": "114.",
  "anya-e1005n0:587_0": "Yasmiṃ padese na māno,",
  "anya-e1005n0:588_0": "Na pemaṃ naca bandhavā;",
  "anya-e1005n0:589_0": "Naca vijjāgamo koci,",
  "anya-e1005n0:590_0": "Na tattha divasaṃ vase.",
  "anya-e1005n0:591_0": "115.",
  "anya-e1005n0:592_0": "Aputtakaṃ gharaṃ suññaṃ,",
  "anya-e1005n0:593_0": "Raṭṭhaṃ suññaṃ arājakaṃ;",
  "anya-e1005n0:594_0": "A sippassa mukhaṃ suññaṃ,",
  "anya-e1005n0:595_0": "Sabba suññaṃ daliddakā.",
  "anya-e1005n0:596_0": "116.",
  "anya-e1005n0:597_0": "Dhanamiccheyya vāṇijjo,",
  "anya-e1005n0:598_0": "Vijjamicche bhajesutaṃ;",
  "anya-e1005n0:599_0": "Puttamicche taruṇitthiṃ,",
  "anya-e1005n0:600_0": "Rājāmaccaṃ vasaṃ game.",
  "anya-e1005n0:601_0": "117.",
  "anya-e1005n0:602_0": "Naṭṭhoyati asantuṭṭho,",
  "anya-e1005n0:603_0": "Santuṭṭho ca mahīpati;",
  "anya-e1005n0:604_0": "Lajjā ca gaṇikā naṭṭhā,",
  "anya-e1005n0:605_0": "Nillajjā kuladhītikā.",
  "anya-e1005n0:606_0": "118.",
  "anya-e1005n0:607_0": "Pakkhīnaṃ balamākāso,",
  "anya-e1005n0:608_0": "Macchānamudakaṃ balaṃ;",
  "anya-e1005n0:609_0": "Dubbalassa balaṃ rājā,",
  "anya-e1005n0:610_0": "Kumārānaṃ rudaṃ balaṃ.",
  "anya-e1005n0:611_0": "119.",
  "anya-e1005n0:612_0": "Khamā jāgariyuṭṭhānaṃ,",
  "anya-e1005n0:613_0": "Saṃvibhāgo dayikkhaṇā;",
  "anya-e1005n0:614_0": "Nāyakassa guṇā ete,",
  "anya-e1005n0:615_0": "Icchitabbā sataṃ guṇā.",
  "anya-e1005n0:616_0": "120.",
  "anya-e1005n0:617_0": "Sakiṃ vadanti rājāno,",
  "anya-e1005n0:618_0": "Sakiṃ samaṇabrāhmaṇā;",
  "anya-e1005n0:619_0": "Sakiṃ sappurisā loke,",
  "anya-e1005n0:620_0": "Esa dhammo sanantano.",
  "anya-e1005n0:621_0": "121.",
  "anya-e1005n0:622_0": "Alaso gihī kāmabhogī na sādhu,",
  "anya-e1005n0:623_0": "Asaññato pabbajito na sādhu;",
  "anya-e1005n0:624_0": "Rājā anisammakārī na sādhu,",
  "anya-e1005n0:625_0": "Paṇḍito kodhano taṃpi na sādhu.",
  "anya-e1005n0:626_0": "122.",
  "anya-e1005n0:627_0": "Bahavo yattha nettāro,",
  "anya-e1005n0:628_0": "Sabbe paṇḍitamānino;",
  "anya-e1005n0:629_0": "Sabbe mahattamicchanti,",
  "anya-e1005n0:630_0": "Tesaṃ kammaṃ vinassati.",
  "anya-e1005n0:631_0": "123.",
  "anya-e1005n0:632_0": "Āyaṃ khayaṃ sayaṃ jaññā,",
  "anya-e1005n0:633_0": "Rājā sayaṃ katākataṃ;",
  "anya-e1005n0:634_0": "Niggahe niggahetabbaṃ,",
  "anya-e1005n0:635_0": "Paggahe paggahārahaṃ.",
  "anya-e1005n0:636_0": "124.",
  "anya-e1005n0:637_0": "Piṭṭhitokkaṃ niseveyya,",
  "anya-e1005n0:638_0": "Kucchināva hutāsanaṃ;",
  "anya-e1005n0:639_0": "Sāmikaṃ sabbabhāgena,",
  "anya-e1005n0:640_0": "Paralokaṃ amohavā.",
  "anya-e1005n0:641_0": "125.",
  "anya-e1005n0:642_0": "Aggi āpo itthimūḷho,",
  "anya-e1005n0:643_0": "Sappo rāja-kulānica;",
  "anya-e1005n0:644_0": "Apayantena gantabbā,",
  "anya-e1005n0:645_0": "Acceka-pāṇahārakā.",
  "anya-e1005n0:646_0": "126.",
  "anya-e1005n0:647_0": "Paduṭṭha-bhariya-saṃvāso,",
  "anya-e1005n0:648_0": "Paduṭṭha citta dāsako;",
  "anya-e1005n0:649_0": "Sa-sappe ca ghare vāso,",
  "anya-e1005n0:650_0": "Maccu eva na saṃsayo.",
  "anya-e1005n0:651_0": "127.",
  "anya-e1005n0:652_0": "Mūḷha sisso padesena,",
  "anya-e1005n0:653_0": "Kunārī bharaṇena ca;",
  "anya-e1005n0:654_0": "Asatā sampayogena,",
  "anya-e1005n0:655_0": "Paṇḍitoppavasīdati.",
  "anya-e1005n0:656_0": "128.",
  "anya-e1005n0:657_0": "Mātā puttakaraṃ pāpaṃ,",
  "anya-e1005n0:658_0": "Sissapāpaṃ gurukatā;",
  "anya-e1005n0:659_0": "Rājā raṭṭhakaraṃ pāpaṃ,",
  "anya-e1005n0:660_0": "Rājapāpaṃ purohito.",
  "anya-e1005n0:661_0": "129.",
  "anya-e1005n0:662_0": "Akodhena jine kodhaṃ,",
  "anya-e1005n0:663_0": "Asādhuṃ sādhunā jine;",
  "anya-e1005n0:664_0": "Jine macchariṃ dānena,",
  "anya-e1005n0:665_0": "Saccenālīkavādinaṃ;",
  "anya-e1005n0:666_0": "130.",
  "anya-e1005n0:667_0": "Adantaṃ damanaṃ dānaṃ,",
  "anya-e1005n0:668_0": "Dānaṃ sabbattha sādhakaṃ;",
  "anya-e1005n0:669_0": "Dānena piya vācāya,",
  "anya-e1005n0:670_0": "Unnamanti namanti ca;",
  "anya-e1005n0:671_0": "131.",
  "anya-e1005n0:672_0": "Dānaṃ sinehabhesajjaṃ,",
  "anya-e1005n0:673_0": "Maccheraṃ dosanosadhaṃ;",
  "anya-e1005n0:674_0": "Dānaṃ yasassī bhesajjaṃ,",
  "anya-e1005n0:675_0": "Maccheraṃ kapaṇosadhaṃ.",
  "anya-e1005n0:676_0": "132.",
  "anya-e1005n0:677_0": "Bahūnamappasārānaṃ,",
  "anya-e1005n0:678_0": "Sāmaggiyā jayaṃ jaye;",
  "anya-e1005n0:679_0": "Tiṇehi vattate yottaṃ,",
  "anya-e1005n0:680_0": "Tena nāgopi bajjhate.",
  "anya-e1005n0:681_0": "133.",
  "anya-e1005n0:682_0": "Sahāyo asamatthopi,",
  "anya-e1005n0:683_0": "Tejasā kiṃkarissati;",
  "anya-e1005n0:684_0": "Nivāte jalito aggi,",
  "anya-e1005n0:685_0": "Sayame vūpasampati.",
  "anya-e1005n0:686_0": "134.",
  "anya-e1005n0:687_0": "Na raññā samakaṃ bhuñje,",
  "anya-e1005n0:688_0": "Kāmabhogaṃ kudācanaṃ;",
  "anya-e1005n0:689_0": "Ākappaṃ rasa bhuttiṃvā,",
  "anya-e1005n0:690_0": "Mālā gandha vilepanaṃ;",
  "anya-e1005n0:691_0": "Vatthaṃ sabbaalaṅkāraṃ,",
  "anya-e1005n0:692_0": "Na raññā sadisaṃ kare.",
  "anya-e1005n0:693_0": "135.",
  "anya-e1005n0:694_0": "Na me rājā sakhā hoti,",
  "anya-e1005n0:695_0": "Na rājā hoti methuno;",
  "anya-e1005n0:696_0": "Eso sāmiko mayhanti,",
  "anya-e1005n0:697_0": "Citte niṭṭhaṃ suthāpaye.",
  "anya-e1005n0:698_0": "136.",
  "anya-e1005n0:699_0": "Nātidūre bhaje rañño,",
  "anya-e1005n0:700_0": "Nāccāsannopavātake;",
  "anya-e1005n0:701_0": "Ujuke nātininne ca,",
  "anya-e1005n0:702_0": "Na bhaje uccamāsane.",
  "anya-e1005n0:703_0": "Chadose vajje sevako,",
  "anya-e1005n0:704_0": "Tiṭṭhe aggiṃva saṃyato.",
  "anya-e1005n0:705_0": "137.",
  "anya-e1005n0:706_0": "Guṇī sabbaññu tulyopi,",
  "anya-e1005n0:707_0": "Nasobhati anissayo;",
  "anya-e1005n0:708_0": "Anagghamopi maṇiseṭṭho,",
  "anya-e1005n0:709_0": "Hemaṃ nissāya sobhati.",
  "anya-e1005n0:710_0": "Rājakaṇḍo niṭṭhito.",
  "anya-e1005n0:711_0": "7. Pakiṇṇakakaṇḍo",
  "anya-e1005n0:712_0": "138.",
  "anya-e1005n0:713_0": "Itthimisse kutosīlaṃ,",
  "anya-e1005n0:714_0": "Maṃsa bhakkhe kutodayā;",
  "anya-e1005n0:715_0": "Surā pāne kutosaccaṃ,",
  "anya-e1005n0:716_0": "Mahālobhe kutohirī;",
  "anya-e1005n0:717_0": "Mahātande kutosippaṃ,",
  "anya-e1005n0:718_0": "Mahā kodhe kutodhanaṃ.",
  "anya-e1005n0:719_0": "139.",
  "anya-e1005n0:720_0": "Surā yogo vikālo ca,",
  "anya-e1005n0:721_0": "Samajja caraṇālasaṃ;",
  "anya-e1005n0:722_0": "Khiḍḍādhutto pāpamitto,",
  "anya-e1005n0:723_0": "Bhoganāsamukhā ime.",
  "anya-e1005n0:724_0": "140.",
  "anya-e1005n0:725_0": "Divā nādikkhā vattabbaṃ,",
  "anya-e1005n0:726_0": "Ratto nāvacanena ca;",
  "anya-e1005n0:727_0": "Sañcareyya bhayā bhīto,",
  "anya-e1005n0:728_0": "Vane vanacarī yathā.",
  "anya-e1005n0:729_0": "141.",
  "anya-e1005n0:730_0": "Jīvantāpi matāpañca,",
  "anya-e1005n0:731_0": "Byāsena parikittitā;",
  "anya-e1005n0:732_0": "Dukkhito byādhitomūḷho,",
  "anya-e1005n0:733_0": "Iṇavā nityasevako.",
  "anya-e1005n0:734_0": "142.",
  "anya-e1005n0:735_0": "Anāgataṃ bhayaṃ disvā,",
  "anya-e1005n0:736_0": "Dūrato parivajjaye;",
  "anya-e1005n0:737_0": "Āgatañca bhayaṃ disvā,",
  "anya-e1005n0:738_0": "A bhīto hoti paṇḍito.",
  "anya-e1005n0:739_0": "143.",
  "anya-e1005n0:740_0": "Niddāluko pamattoca,",
  "anya-e1005n0:741_0": "Sukhatto rogavālaso;",
  "anya-e1005n0:742_0": "Mahiccho kammārāmoca,",
  "anya-e1005n0:743_0": "Satte te satthavajjitā.",
  "anya-e1005n0:744_0": "144.",
  "anya-e1005n0:745_0": "Duggataṃ gaccha he lābha,",
  "anya-e1005n0:746_0": "Lābhī lābhena pūrati;",
  "anya-e1005n0:747_0": "Thale pavassa pajjunna,",
  "anya-e1005n0:748_0": "Sindhu āpena pūrati;",
  "anya-e1005n0:749_0": "Natthidaṃ kammappadhānakaṃ.",
  "anya-e1005n0:750_0": "145.",
  "anya-e1005n0:751_0": "Na hi koci kate kicce,",
  "anya-e1005n0:752_0": "Kattāraṃ samupekkhate;",
  "anya-e1005n0:753_0": "Tasmā sabbāni kiccāni,",
  "anya-e1005n0:754_0": "Sāva sesena kāraye.",
  "anya-e1005n0:755_0": "146.",
  "anya-e1005n0:756_0": "Tūlaṃ sallahukaṃ loke,",
  "anya-e1005n0:757_0": "Tato cāpalla-jātiko;",
  "anya-e1005n0:758_0": "Tato vuḍḍha manovādo,",
  "anya-e1005n0:759_0": "Pamatto buddhasāsane.",
  "anya-e1005n0:760_0": "147.",
  "anya-e1005n0:761_0": "Pāsāṇachattaṃ garukaṃ,",
  "anya-e1005n0:762_0": "Tato devānacikkhaṇaṃ;",
  "anya-e1005n0:763_0": "Tato vuḍḍhānamovādo,",
  "anya-e1005n0:764_0": "Tato buddhassa sāsanaṃ.",
  "anya-e1005n0:765_0": "148.",
  "anya-e1005n0:766_0": "Kāyassa dakkhiṇa hattho,",
  "anya-e1005n0:767_0": "Doso ettha kaniṭṭhako;",
  "anya-e1005n0:768_0": "Kaṇṇa ghānāna-makkhīnaṃ,",
  "anya-e1005n0:769_0": "Vāmo tu pāda-pāsako.",
  "anya-e1005n0:770_0": "149.",
  "anya-e1005n0:771_0": "Tambūlassa majjha patte,",
  "anya-e1005n0:772_0": "Kuvero rakkhatī sadā;",
  "anya-e1005n0:773_0": "Mūlamhi rakkhati yakkho,",
  "anya-e1005n0:774_0": "Aggamhi kālakaṇṇikā;",
  "anya-e1005n0:775_0": "Tāni bhuñjeyya chinditvā,",
  "anya-e1005n0:776_0": "Sirī evaṃ pavaḍḍhati.",
  "anya-e1005n0:777_0": "150.",
  "anya-e1005n0:778_0": "Sampuṇṇarakkho brahmāva,",
  "anya-e1005n0:779_0": "Accurakkho ca bissaṇo;",
  "anya-e1005n0:780_0": "Tasmā hi te pūjayantu,",
  "anya-e1005n0:781_0": "Sadā mānenti taṃ naraṃ.",
  "anya-e1005n0:782_0": "151.",
  "anya-e1005n0:783_0": "Goṇā hi sabbagihīnaṃ,",
  "anya-e1005n0:784_0": "Posakā bhogadāyakā;",
  "anya-e1005n0:785_0": "Tasmā hi mātā pitūva,",
  "anya-e1005n0:786_0": "Mānaye sakkareyya ca.",
  "anya-e1005n0:787_0": "152.",
  "anya-e1005n0:788_0": "Yeca khādanti gomaṃsaṃ,",
  "anya-e1005n0:789_0": "Mātu maṃsaṃva khādare;",
  "anya-e1005n0:790_0": "Matesu tesu gijjhānaṃ,",
  "anya-e1005n0:791_0": "Dade sote ca vāhaye.",
  "anya-e1005n0:792_0": "153.",
  "anya-e1005n0:793_0": "Gurusiddho sippārambho,",
  "anya-e1005n0:794_0": "Ravi sokrā ca majjhimo;",
  "anya-e1005n0:795_0": "Na sippo buddhacandaro,",
  "anya-e1005n0:796_0": "Sorī aṅgāca maraṇaṃ.",
  "anya-e1005n0:797_0": "154.",
  "anya-e1005n0:798_0": "Aṭṭhamiyaṃ guruṃ hanti,",
  "anya-e1005n0:799_0": "Sissaṃ hanti catuddasiṃ;",
  "anya-e1005n0:800_0": "Sippaṃ hanti dasa sippaṃ,",
  "anya-e1005n0:801_0": "Mātāpitā ca puṇṇamiṃ.",
  "anya-e1005n0:802_0": "155.",
  "anya-e1005n0:803_0": "Nāḷikaṃ satta nabhuñje,",
  "anya-e1005n0:804_0": "Na lābuṃ navamaṃ tathā;",
  "anya-e1005n0:805_0": "Dvādasa prinnaṃtriminaṃ,",
  "anya-e1005n0:806_0": "Bhuñje sippaṃ vinassati.",
  "anya-e1005n0:807_0": "156.",
  "anya-e1005n0:808_0": "Ekaṃ caje kulaatthaṃ,",
  "anya-e1005n0:809_0": "Gāmassatthaṃ kuvaṃ caje;",
  "anya-e1005n0:810_0": "Gāma caje janapadatthaṃ,",
  "anya-e1005n0:811_0": "Attatthaṃ pathaviṃ caje.",
  "anya-e1005n0:812_0": "157.",
  "anya-e1005n0:813_0": "Desaṃ ossajja gacchanti,",
  "anya-e1005n0:814_0": "Sīho sappuriso gajo;",
  "anya-e1005n0:815_0": "Tattheva nidhanaṃ yanti,",
  "anya-e1005n0:816_0": "Kāko kāpuriso migo.",
  "anya-e1005n0:817_0": "158.",
  "anya-e1005n0:818_0": "Yamhi padese na māno,",
  "anya-e1005n0:819_0": "Na pemaṃ na ca bandhavā;",
  "anya-e1005n0:820_0": "Na ca vijjāgāho koci,",
  "anya-e1005n0:821_0": "Na tattha vasanaṃ kare.",
  "anya-e1005n0:822_0": "159.",
  "anya-e1005n0:823_0": "Caratyekena pādena,",
  "anya-e1005n0:824_0": "Tiṭṭhatyekena paṇḍito;",
  "anya-e1005n0:825_0": "A nisamma paraṃ ṭhānaṃ,",
  "anya-e1005n0:826_0": "Na pubbamālayaṃ jahe.",
  "anya-e1005n0:827_0": "160.",
  "anya-e1005n0:828_0": "Dhana dhañña payogesu,",
  "anya-e1005n0:829_0": "Tathā vijjāgamesu ca;",
  "anya-e1005n0:830_0": "Dūtesu apacāresu,",
  "anya-e1005n0:831_0": "Cajjā lajjā tadā bhave.",
  "anya-e1005n0:832_0": "161.",
  "anya-e1005n0:833_0": "Dvi guṇo thīnamāhāro,",
  "anya-e1005n0:834_0": "Buddhicāpi catugguṇo;",
  "anya-e1005n0:835_0": "Chagguṇo hoti vāyāmo,",
  "anya-e1005n0:836_0": "Kāmotvaṭṭha-guṇo bhave.",
  "anya-e1005n0:837_0": "162.",
  "anya-e1005n0:838_0": "Pabbe pabbe kamenucchu,",
  "anya-e1005n0:839_0": "Visesarasavāggato;",
  "anya-e1005n0:840_0": "Tathā sumettiko sādhu,",
  "anya-e1005n0:841_0": "Viparītova dujjano.",
  "anya-e1005n0:842_0": "163.",
  "anya-e1005n0:843_0": "Kassako vāṇijo macco,",
  "anya-e1005n0:844_0": "Samaṇo suta sīlavā;",
  "anya-e1005n0:845_0": "Tesu vipula jātesu,",
  "anya-e1005n0:846_0": "Raṭṭhampi vipulaṃ siyā.",
  "anya-e1005n0:847_0": "164.",
  "anya-e1005n0:848_0": "Asajjhāya malā mantā,",
  "anya-e1005n0:849_0": "Anuṭṭhāna malā gharā;",
  "anya-e1005n0:850_0": "Malaṃ vaṇṇassa kosajjaṃ,",
  "anya-e1005n0:851_0": "Pamādo rakkhato malaṃ.",
  "anya-e1005n0:852_0": "165.",
  "anya-e1005n0:853_0": "Hīnānaṃ gacchate vittaṃ,",
  "anya-e1005n0:854_0": "Vīrānaṃ santakattanaṃ;",
  "anya-e1005n0:855_0": "Vadanti ca hīnā janā,",
  "anya-e1005n0:856_0": "Pubba-kammappadhānakā.",
  "anya-e1005n0:857_0": "166.",
  "anya-e1005n0:858_0": "Na vadanti cevaṃdhīrā,",
  "anya-e1005n0:859_0": "Vāyamiṃsu sabbakamme;",
  "anya-e1005n0:860_0": "Na ce sijjhati taṃ kammaṃ,",
  "anya-e1005n0:861_0": "A-phalaṃ eva ko doso.",
  "anya-e1005n0:862_0": "167.",
  "anya-e1005n0:863_0": "Nīcaṃ kulaṃ nipaññaṃ vā,",
  "anya-e1005n0:864_0": "Nirūpaṃ nibalaṃ samaṃ;",
  "anya-e1005n0:865_0": "Imaṃ kālaṃ chuttakālaṃ,",
  "anya-e1005n0:866_0": "Dhanameva visesakaṃ.",
  "anya-e1005n0:867_0": "Pakiṇṇakakaṇḍo niṭṭhito.",
  "anya-e1005n0:868_0": "Paṇḍito sujano kaṇḍo,",
  "anya-e1005n0:869_0": "Dujjano mitta-itthi ca;",
  "anya-e1005n0:870_0": "Rājā pakiṇṇako cāti,",
  "anya-e1005n0:871_0": "Satta-kaṇḍa-vibhūsitaṃ.",
  "anya-e1005n0:872_0": "Visuddhā cāra-therena,",
  "anya-e1005n0:873_0": "Visuddhārāma-vāsinā;",
  "anya-e1005n0:874_0": "Sabba-kulānamatthāya,",
  "anya-e1005n0:875_0": "Visodhitaṃ pathakkhaye."
}