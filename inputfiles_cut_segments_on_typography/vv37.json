{
  "vv37:0.1_0": "Vimānavatthu",
  "vv37:0.2_0": "Itthivimāna",
  "vv37:0.3_0": "Pāricchattakavagga",
  "vv37:0.4_0": "9. Visālakkhivimānavatthu",
  "vv37:1.1_0": "“Kā nāma tvaṁ visālakkhi,",
  "vv37:1.2_0": "ramme cittalatāvane;",
  "vv37:1.3_0": "Samantā anupariyāsi,",
  "vv37:1.4_0": "nārīgaṇapurakkhatā.",
  "vv37:2.1_0": "Yadā devā tāvatiṁsā,",
  "vv37:2.2_0": "pavisanti imaṁ vanaṁ;",
  "vv37:2.3_0": "Sayoggā sarathā sabbe,",
  "vv37:2.4_0": "citrā honti idhāgatā.",
  "vv37:3.1_0": "Tuyhañca idha pattāya,",
  "vv37:3.2_0": "uyyāne vicarantiyā;",
  "vv37:3.3_0": "Kāye na dissatī cittaṁ,",
  "vv37:3.4_0": "kena rūpaṁ tavedisaṁ;",
  "vv37:3.5_0": "Devate pucchitācikkha,",
  "vv37:3.6_0": "kissa kammassidaṁ phalan”ti.",
  "vv37:4.1_0": "“Yena kammena devinda,",
  "vv37:4.2_0": "rūpaṁ mayhaṁ gatī ca me;",
  "vv37:4.3_0": "Iddhi ca ānubhāvo ca,",
  "vv37:4.4_0": "taṁ suṇohi purindada.",
  "vv37:5.1_0": "Ahaṁ rājagahe ramme,",
  "vv37:5.2_0": "sunandā nāmupāsikā;",
  "vv37:5.3_0": "Saddhā sīlena sampannā,",
  "vv37:5.4_0": "saṁvibhāgaratā sadā.",
  "vv37:6.1_0": "Acchādanañca bhattañca,",
  "vv37:6.2_0": "senāsanaṁ padīpiyaṁ;",
  "vv37:6.3_0": "Adāsiṁ ujubhūtesu,",
  "vv37:6.4_0": "vippasannena cetasā.",
  "vv37:7.1_0": "Cātuddasiṁ pañcadasiṁ,",
  "vv37:7.2_0": "yā ca pakkhassa aṭṭhamī;",
  "vv37:7.3_0": "Pāṭihāriyapakkhañca,",
  "vv37:7.4_0": "aṭṭhaṅgasusamāgataṁ.",
  "vv37:8.1_0": "Uposathaṁ upavasissaṁ,",
  "vv37:8.2_0": "sadā sīlesu saṁvutā;",
  "vv37:8.3_0": "Saññamā saṁvibhāgā ca,",
  "vv37:8.4_0": "vimānaṁ āvasāmahaṁ.",
  "vv37:9.1_0": "Pāṇātipātā viratā,",
  "vv37:9.2_0": "musāvādā ca saññatā;",
  "vv37:9.3_0": "Theyyā ca aticārā ca,",
  "vv37:9.4_0": "majjapānā ca ārakā.",
  "vv37:10.1_0": "Pañcasikkhāpade ratā,",
  "vv37:10.2_0": "ariyasaccāna kovidā;",
  "vv37:10.3_0": "Upāsikā cakkhumato,",
  "vv37:10.4_0": "gotamassa yasassino.",
  "vv37:11.1_0": "Tassā me ñātikulā dāsī,",
  "vv37:11.2_0": "sadā mālābhihārati;",
  "vv37:11.3_0": "Tāhaṁ bhagavato thūpe,",
  "vv37:11.4_0": "sabbamevābhiropayiṁ.",
  "vv37:12.1_0": "Uposathe cahaṁ gantvā,",
  "vv37:12.2_0": "mālāgandhavilepanaṁ;",
  "vv37:12.3_0": "Thūpasmiṁ abhiropesiṁ,",
  "vv37:12.4_0": "pasannā sehi pāṇibhi.",
  "vv37:13.1_0": "Tena kammena devinda,",
  "vv37:13.2_0": "rūpaṁ mayhaṁ gatī ca me;",
  "vv37:13.3_0": "Iddhī ca ānubhāvo ca,",
  "vv37:13.4_0": "yaṁ mālaṁ abhiropayiṁ.",
  "vv37:14.1_0": "Yañca sīlavatī āsiṁ,",
  "vv37:14.2_0": "na taṁ tāva vipaccati;",
  "vv37:14.3_0": "Āsā ca pana me devinda,",
  "vv37:14.4_0": "sakadāgāminī siyan”ti.",
  "vv37:15.1_0": "Visālakkhivimānaṁ navamaṁ."
}