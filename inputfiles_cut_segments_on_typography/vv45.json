{
  "vv45:0.1_0": "Vimānavatthu",
  "vv45:0.2_0": "Itthivimāna",
  "vv45:0.3_0": "Mañjiṭṭhakavagga",
  "vv45:0.4_0": "7. Caturitthivimānavatthu",
  "vv45:1.1_0": "“Abhikkantena vaṇṇena,",
  "vv45:1.2_0": "…pe…",
  "vv45:1.3_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv45:2.1_0": "Sā devatā attamanā,",
  "vv45:2.2_0": "…pe…",
  "vv45:2.3_0": "yassa kammassidaṁ phalaṁ.",
  "vv45:3.1_0": "“Indīvarānaṁ hatthakaṁ ahamadāsiṁ,",
  "vv45:3.2_0": "Bhikkhuno piṇḍāya carantassa;",
  "vv45:3.3_0": "Esikānaṁ uṇṇatasmiṁ,",
  "vv45:3.4_0": "Nagaravare paṇṇakate ramme.",
  "vv45:4.1_0": "Tena metādiso vaṇṇo,",
  "vv45:4.2_0": "…pe…",
  "vv45:4.3_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv45:5.1_0": "“Abhikkantena vaṇṇena,",
  "vv45:5.2_0": "…pe…",
  "vv45:5.3_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv45:6.1_0": "Sā devatā attamanā,",
  "vv45:6.2_0": "…pe…",
  "vv45:6.3_0": "yassa kammassidaṁ phalaṁ.",
  "vv45:7.1_0": "“Nīluppalahatthakaṁ ahamadāsiṁ,",
  "vv45:7.2_0": "Bhikkhuno piṇḍāya carantassa;",
  "vv45:7.3_0": "Esikānaṁ uṇṇatasmiṁ,",
  "vv45:7.4_0": "Nagaravare paṇṇakate ramme.",
  "vv45:8.1_0": "Tena metādiso vaṇṇo,",
  "vv45:8.2_0": "…pe…",
  "vv45:8.3_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv45:9.1_0": "“Abhikkantena vaṇṇena,",
  "vv45:9.2_0": "…pe…",
  "vv45:9.3_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv45:10.1_0": "Sā devatā attamanā,",
  "vv45:10.2_0": "…pe…",
  "vv45:10.3_0": "yassa kammassidaṁ phalaṁ.",
  "vv45:11.1_0": "“Odātamūlakaṁ haritapattaṁ,",
  "vv45:11.2_0": "Udakasmiṁ sare jātaṁ ahamadāsiṁ;",
  "vv45:11.3_0": "Bhikkhuno piṇḍāya carantassa,",
  "vv45:11.4_0": "Esikānaṁ uṇṇatasmiṁ;",
  "vv45:11.5_0": "Nagaravare paṇṇakate ramme.",
  "vv45:12.1_0": "Tena metādiso vaṇṇo,",
  "vv45:12.2_0": "…pe…",
  "vv45:12.3_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv45:13.1_0": "“Abhikkantena vaṇṇena,",
  "vv45:13.2_0": "…pe…",
  "vv45:13.3_0": "Vaṇṇo ca te sabbadisā pabhāsatī”ti.",
  "vv45:14.1_0": "Sā devatā attamanā,",
  "vv45:14.2_0": "…pe…",
  "vv45:14.3_0": "yassa kammassidaṁ phalaṁ.",
  "vv45:15.1_0": "“Ahaṁ sumanā sumanassa sumanamakuḷāni,",
  "vv45:15.2_0": "Dantavaṇṇāni ahamadāsiṁ;",
  "vv45:15.3_0": "Bhikkhuno piṇḍāya carantassa,",
  "vv45:15.4_0": "Esikānaṁ uṇṇatasmiṁ;",
  "vv45:15.5_0": "Nagaravare paṇṇakate ramme.",
  "vv45:16.1_0": "Tena metādiso vaṇṇo,",
  "vv45:16.2_0": "…pe…",
  "vv45:16.3_0": "Vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv45:17.1_0": "Caturitthivimānaṁ sattamaṁ."
}