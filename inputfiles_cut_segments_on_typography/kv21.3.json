{
  "kv21.3:0.1_0": "Kathāvatthu",
  "kv21.3:0.2_0": "Mahāpaṇṇāsaka",
  "kv21.3:0.3_0": "Ekavīsatimavagga",
  "kv21.3:0.4_0": "Saṁyojanakathā",
  "kv21.3:1.1_0": "Atthi kiñci saṁyojanaṁ appahāya arahattappattīti?",
  "kv21.3:1.2_0": "Āmantā.",
  "kv21.3:1.3_0": "Atthi kiñci sakkāyadiṭṭhiṁ appahāya …pe…",
  "kv21.3:1.4_0": "vicikicchaṁ appahāya …pe…",
  "kv21.3:1.5_0": "sīlabbataparāmāsaṁ appahāya …",
  "kv21.3:1.6_0": "rāgaṁ appahāya …",
  "kv21.3:1.7_0": "dosaṁ appahāya …",
  "kv21.3:1.8_0": "mohaṁ appahāya …",
  "kv21.3:1.9_0": "anottappaṁ appahāya arahattappattīti?",
  "kv21.3:1.10_0": "Na hevaṁ vattabbe …pe….",
  "kv21.3:2.1_0": "Atthi kiñci saṁyojanaṁ appahāya arahattappattīti?",
  "kv21.3:2.2_0": "Āmantā.",
  "kv21.3:2.3_0": "Arahā sarāgo sadoso samoho samāno samakkho sapaḷāso saupāyāso sakilesoti?",
  "kv21.3:2.4_0": "Na hevaṁ vattabbe …pe…",
  "kv21.3:2.5_0": "nanu arahā nirāgo niddoso nimmoho nimmāno nimmakkho nippaḷāso nirupāyāso nikkilesoti?",
  "kv21.3:2.6_0": "Āmantā.",
  "kv21.3:2.7_0": "Hañci arahā nirāgo …pe…",
  "kv21.3:2.8_0": "nikkileso, no ca vata re vattabbe—",
  "kv21.3:2.9_0": "“atthi kiñci saṁyojanaṁ appahāya arahattappattī”ti.",
  "kv21.3:3.1_0": "Na vattabbaṁ—",
  "kv21.3:3.2_0": "“kiñci saṁyojanaṁ appahāya arahattappattī”ti?",
  "kv21.3:3.3_0": "Āmantā.",
  "kv21.3:3.4_0": "Arahā sabbaṁ buddhavisayaṁ jānātīti?",
  "kv21.3:3.5_0": "Na hevaṁ vattabbe.",
  "kv21.3:3.6_0": "Tena hi atthi kiñci saṁyojanaṁ appahāya arahattappattīti.",
  "kv21.3:3.7_0": "Saṁyojanakathā niṭṭhitā."
}