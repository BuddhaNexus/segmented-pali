{
  "ja27:0.1_0": "Jātaka",
  "ja27:0.2_0": "Ekakanipāta",
  "ja27:0.3_0": "Kuruṅgavagga",
  "ja27:0.4_0": "7. Abhiṇhajātaka",
  "ja27:1.1_0": "“Nālaṁ kabaḷaṁ padātave,",
  "ja27:1.2_0": "Na ca piṇḍaṁ na kuse na ghaṁsituṁ;",
  "ja27:1.3_0": "Maññāmi abhiṇhadassanā,",
  "ja27:1.4_0": "Nāgo snehamakāsi kukkure”ti.",
  "ja27:2.1_0": "Abhiṇhajātakaṁ sattamaṁ."
}