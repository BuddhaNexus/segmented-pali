{
  "thag2.24:0.1_0": "Theragāthā 2.24",
  "thag2.24:0.2_0": "Dukanipāta",
  "thag2.24:0.3_0": "Tatiyavagga",
  "thag2.24:0.4_0": "Valliyattheragāthā",
  "thag2.24:1.1_0": "“Yaṁ kiccaṁ daḷhavīriyena,",
  "thag2.24:1.2_0": "yaṁ kiccaṁ boddhumicchatā;",
  "thag2.24:1.3_0": "Karissaṁ nāvarajjhissaṁ,",
  "thag2.24:1.4_0": "passa vīriyaṁ parakkamaṁ.",
  "thag2.24:2.1_0": "Tvañca me maggamakkhāhi,",
  "thag2.24:2.2_0": "añjasaṁ amatogadhaṁ;",
  "thag2.24:2.3_0": "Ahaṁ monena monissaṁ,",
  "thag2.24:2.4_0": "gaṅgāsotova sāgaran”ti.",
  "thag2.24:2.5_0": "… Valliyo thero …."
}