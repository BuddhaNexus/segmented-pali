{
  "tha-ap102:0.1_0": "Therāpadāna",
  "tha-ap102:0.2_0": "Sudhāvagga",
  "tha-ap102:0.3_0": "10. Buddhupaṭṭhākattheraapadāna",
  "tha-ap102:1.1_0": "“Vipassissa bhagavato,",
  "tha-ap102:1.2_0": "ahosiṁ saṅkhadhammako;",
  "tha-ap102:1.3_0": "Niccupaṭṭhānayuttomhi,",
  "tha-ap102:1.4_0": "sugatassa mahesino.",
  "tha-ap102:2.1_0": "Upaṭṭhānaphalaṁ passa,",
  "tha-ap102:2.2_0": "lokanāthassa tādino;",
  "tha-ap102:2.3_0": "Saṭṭhitūriyasahassāni,",
  "tha-ap102:2.4_0": "parivārenti maṁ sadā.",
  "tha-ap102:3.1_0": "Ekanavutito kappe,",
  "tha-ap102:3.2_0": "upaṭṭhahiṁ mahāisiṁ;",
  "tha-ap102:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap102:3.4_0": "upaṭṭhānassidaṁ phalaṁ.",
  "tha-ap102:4.1_0": "Catuvīse ito kappe,",
  "tha-ap102:4.2_0": "mahānigghosanāmakā;",
  "tha-ap102:4.3_0": "Soḷasāsiṁsu rājāno,",
  "tha-ap102:4.4_0": "cakkavattī mahabbalā.",
  "tha-ap102:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap102:5.2_0": "…pe…",
  "tha-ap102:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap102:6.1_0": "Itthaṁ sudaṁ āyasmā buddhupaṭṭhāko thero imā gāthāyo abhāsitthāti.",
  "tha-ap102:7.1_0": "Buddhupaṭṭhākattherassāpadānaṁ dasamaṁ.",
  "tha-ap102:7.2_0": "Sudhāvaggo dasamo.",
  "tha-ap102:8.1_0": "Tassuddānaṁ",
  "tha-ap102:9.1_0": "Sudhā sucinti ceḷañca,",
  "tha-ap102:9.2_0": "sūcī ca gandhamāliyo;",
  "tha-ap102:9.3_0": "Tipupphiyo madhusenā,",
  "tha-ap102:9.4_0": "veyyāvacco cupaṭṭhako;",
  "tha-ap102:9.5_0": "Samasaṭṭhi ca gāthāyo,",
  "tha-ap102:9.6_0": "asmiṁ vagge pakittitā.",
  "tha-ap102:10.1_0": "Atha vagguddānaṁ",
  "tha-ap102:11.1_0": "Buddhavaggo hi paṭhamo,",
  "tha-ap102:11.2_0": "sīhāsani subhūti ca;",
  "tha-ap102:11.3_0": "Kuṇḍadhāno upāli ca,",
  "tha-ap102:11.4_0": "bījanisakacinti ca.",
  "tha-ap102:12.1_0": "Nāgasamālo timiro,",
  "tha-ap102:12.2_0": "sudhāvaggena te dasa;",
  "tha-ap102:12.3_0": "Catuddasasatā gāthā,",
  "tha-ap102:12.4_0": "pañcapaññāsameva ca.",
  "tha-ap102:13.1_0": "Buddhavaggadasakaṁ.",
  "tha-ap102:14.1_0": "Paṭhamasatakaṁ samattaṁ."
}