{
  "tika-abh06t48:0_0": "Sattamo paricchedo",
  "tika-abh06t48:1_0": "7. Bhūmipuggalasambhavakathā",
  "tika-abh06t48:2_0": "270.",
  "tika-abh06t48:3_0": "Dvihetukāhetukānaṁ, na sampajjati appanā;",
  "tika-abh06t48:4_0": "Arahattañca natthīti, nattheva javanakriyā.",
  "tika-abh06t48:5_0": "271.",
  "tika-abh06t48:6_0": "Ñāṇapākā na vattanti, jaḷattā mūlasandhiyā;",
  "tika-abh06t48:7_0": "Dvihetukatadālambaṁ, siyā sugatiyaṁ na vā.",
  "tika-abh06t48:8_0": "272.",
  "tika-abh06t48:9_0": "Tihetukānaṁ sattānaṁ, samathañca vipassanaṁ;",
  "tika-abh06t48:10_0": "Bhāventānaṁ pavattanti, chabbīsatipi appanā.",
  "tika-abh06t48:11_0": "273.",
  "tika-abh06t48:12_0": "Arahantāna sattānaṁ, bhavanti javanakriyā;",
  "tika-abh06t48:13_0": "Yathābhūminiyāmena, ñāṇapākā ca labbhare.",
  "tika-abh06t48:14_0": "274.",
  "tika-abh06t48:15_0": "Vajjhā paṭhamamaggena, kaṅkhādiṭṭhiyutā pana;",
  "tika-abh06t48:16_0": "Paṭighaṁ tatiyeneva, kammamantena sāsavaṁ.",
  "tika-abh06t48:17_0": "275.",
  "tika-abh06t48:18_0": "Tasmā tesaṁ na vattanti, tāni cittāni sabbathā;",
  "tika-abh06t48:19_0": "Maggaṭṭhānaṁ tu maggova, nāññaṁ sambhoti kiñcipi.",
  "tika-abh06t48:20_0": "276.",
  "tika-abh06t48:21_0": "Ahetukavipākāni, labbhamānāya vīthiyā;",
  "tika-abh06t48:22_0": "Sabbathāpi ca sabbesaṁ, sambhavanti yathārahaṁ.",
  "tika-abh06t48:23_0": "277.",
  "tika-abh06t48:24_0": "Pañcadvāre manodvāre, dhuvamāvajjanadvayaṁ;",
  "tika-abh06t48:25_0": "Parittapuññāpuññāni, labbhanti lahuvuttito.",
  "tika-abh06t48:26_0": "278.",
  "tika-abh06t48:27_0": "Kriyājavanamappanā, natthāpāyesu kāraṇaṁ;",
  "tika-abh06t48:28_0": "Natthi sahetukā pākā, duggatattā hi sandhiyā.",
  "tika-abh06t48:29_0": "279.",
  "tika-abh06t48:30_0": "Brahmānaṁ paṭighaṁ natthi, jhānavikkhambhitaṁ tathā;",
  "tika-abh06t48:31_0": "Heṭṭhājhānaṁ virattattā, na bhāventi arūpino.",
  "tika-abh06t48:32_0": "280.",
  "tika-abh06t48:33_0": "Pubbeva diṭṭhasaccāva, ariyārūpabhūmakā;",
  "tika-abh06t48:34_0": "Tasmādimaggo natthettha, kāyābhāvā sitaṁ tathā.",
  "tika-abh06t48:35_0": "281.",
  "tika-abh06t48:36_0": "Suddhāvāsāpi pattāva, heṭṭhānuttarapañcakaṁ;",
  "tika-abh06t48:37_0": "Sattapāpapahīnā ca, tasmā natthettha tāni ca.",
  "tika-abh06t48:38_0": "282.",
  "tika-abh06t48:39_0": "Pañcadvārikacittāni, dvārābhāve na vijjare;",
  "tika-abh06t48:40_0": "Sahetukavipākā ca, yathābhūmivavatthitā.",
  "tika-abh06t48:41_0": "283.",
  "tika-abh06t48:42_0": "Sambhavāsambhavañcevaṁ, ñatvā puggalabhūmisu;",
  "tika-abh06t48:43_0": "Labbhamānavasā tattha, cittasaṅgahamuddise.",
  "tika-abh06t48:44_0": "284.",
  "tika-abh06t48:45_0": "Kusalādippabhedā ca, tathā bhūmādibhedato;",
  "tika-abh06t48:46_0": "Vatthudvārārammaṇato, bhūmipuggalatopi ca.",
  "tika-abh06t48:47_0": "285.",
  "tika-abh06t48:48_0": "Vibhāgo yo samuddiṭṭho,",
  "tika-abh06t48:49_0": "Cittānañca tu sambhavā;",
  "tika-abh06t48:50_0": "Ñeyyo cetasikānañca,",
  "tika-abh06t48:51_0": "Sampayogānusāratoti.",
  "tika-abh06t48:52_0": "Iti cittavibhāge bhūmipuggalasambhavakathā niṭṭhitā.",
  "tika-abh06t48:53_0": "Sattamo paricchedo.",
  "tika-abh06t48:54_0": "Niṭṭhito ca cittavibhāgo."
}