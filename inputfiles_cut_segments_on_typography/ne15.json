{
  "ne15:0.1_0": "Netti",
  "ne15:0.2_0": "Paṭiniddesavāra",
  "ne15:0.3_0": "Vibhaṅga 12",
  "ne15:0.4_0": "Otaraṇahāravibhaṅga",
  "ne15:1.1_0": "Tattha katamo otaraṇo hāro?",
  "ne15:1.2_0": "“Yo ca paṭiccuppādo”ti.",
  "ne15:2.1_0": "“Uddhaṁ adho sabbadhi vippamutto,",
  "ne15:2.2_0": "Ayaṁ ahasmīti anānupassī;",
  "ne15:2.3_0": "Evaṁ vimutto udatāri oghaṁ,",
  "ne15:2.4_0": "Atiṇṇapubbaṁ apunabbhavāyā”ti.",
  "ne15:3.1_0": "“Uddhan”ti rūpadhātu ca arūpadhātu ca.",
  "ne15:3.2_0": "“Adho”ti kāmadhātu.",
  "ne15:3.3_0": "“Sabbadhi vippamutto”ti tedhātuke ayaṁ asekkhāvimutti.",
  "ne15:3.4_0": "Tāniyeva asekkhāni pañcindriyāni, ayaṁ indriyehi otaraṇā.",
  "ne15:4.1_0": "Tāniyeva asekkhāni pañcindriyāni vijjā,",
  "ne15:4.1_1": "vijjuppādā avijjānirodho,",
  "ne15:4.1_2": "avijjānirodhā saṅkhāranirodho,",
  "ne15:4.1_3": "saṅkhāranirodhā viññāṇanirodho,",
  "ne15:4.1_4": "viññāṇanirodhā nāmarūpanirodho,",
  "ne15:4.1_5": "nāmarūpanirodhā saḷāyatananirodho,",
  "ne15:4.1_6": "saḷāyatananirodhā phassanirodho,",
  "ne15:4.1_7": "phassanirodhā vedanānirodho,",
  "ne15:4.1_8": "vedanānirodhā taṇhānirodho,",
  "ne15:4.1_9": "taṇhānirodhā upādānanirodho,",
  "ne15:4.1_10": "upādānanirodhā bhavanirodho,",
  "ne15:4.1_11": "bhavanirodhā jātinirodho,",
  "ne15:4.1_12": "jātinirodhā jarāmaraṇaṁ sokaparidevadukkhadomanassupāyāsā nirujjhanti.",
  "ne15:4.2_0": "Evametassa kevalassa dukkhakkhandhassa nirodho hoti.",
  "ne15:4.3_0": "Ayaṁ paṭiccasamuppādehi otaraṇā.",
  "ne15:5.1_0": "Tāniyeva asekkhāni pañcindriyāni tīhi khandhehi saṅgahitāni—",
  "ne15:5.2_0": "sīlakkhandhena samādhikkhandhena paññākkhandhena, ayaṁ khandhehi otaraṇā.",
  "ne15:6.1_0": "Tāniyeva asekkhāni pañcindriyāni saṅkhārapariyāpannāni ye saṅkhārā anāsavā,",
  "ne15:6.1_1": "no ca bhavaṅgā,",
  "ne15:6.1_2": "te saṅkhārā dhammadhātusaṅgahitā.",
  "ne15:6.2_0": "Ayaṁ dhātūhi otaraṇā.",
  "ne15:7.1_0": "Sā dhammadhātu dhammāyatanapariyāpannā, yaṁ āyatanaṁ anāsavaṁ, no ca bhavaṅgaṁ.",
  "ne15:7.2_0": "Ayaṁ āyatanehi otaraṇā.",
  "ne15:8.1_0": "“Ayaṁ ahasmīti anānupassī”ti ayaṁ sakkāyadiṭṭhiyā samugghāto,",
  "ne15:8.1_1": "sā sekkhāvimutti,",
  "ne15:8.1_2": "tāniyeva sekkhāni pañcindriyāni.",
  "ne15:8.2_0": "Ayaṁ indriyehi otaraṇā.",
  "ne15:9.1_0": "Tāniyeva sekkhāni pañcindriyāni vijjā,",
  "ne15:9.1_1": "vijjuppādā avijjānirodho,",
  "ne15:9.1_2": "avijjānirodhā saṅkhāranirodho,",
  "ne15:9.1_3": "evaṁ sabbo paṭiccasamuppādo.",
  "ne15:9.2_0": "Ayaṁ paṭiccasamuppādehi otaraṇā.",
  "ne15:10.1_0": "Sāyeva vijjā paññākkhandho.",
  "ne15:10.2_0": "Ayaṁ khandhehi otaraṇā.",
  "ne15:11.1_0": "Sāyeva vijjā saṅkhārapariyāpannā,",
  "ne15:11.1_1": "ye saṅkhārā anāsavā,",
  "ne15:11.1_2": "no ca bhavaṅgā,",
  "ne15:11.1_3": "te saṅkhārā dhammadhātusaṅgahitā,",
  "ne15:11.1_4": "ayaṁ dhātūhi otaraṇā.",
  "ne15:12.1_0": "Sā dhammadhātu dhammāyatanapariyāpannā,",
  "ne15:12.1_1": "yaṁ āyatanaṁ anāsavaṁ,",
  "ne15:12.1_2": "no ca bhavaṅgaṁ,",
  "ne15:12.1_3": "ayaṁ āyatanehi otaraṇā.",
  "ne15:13.1_0": "Sekkhāya ca vimuttiyā asekkhāya ca vimuttiyā vimutto udatāri oghaṁ atiṇṇapubbaṁ apunabbhavāya.",
  "ne15:13.2_0": "Tenāha bhagavā “uddhaṁ adho”ti.",
  "ne15:14.1_0": "“Nissitassa calitaṁ,",
  "ne15:14.1_1": "anissitassa calitaṁ natthi,",
  "ne15:14.1_2": "calite asati passaddhi,",
  "ne15:14.1_3": "passaddhiyā sati nati na hoti,",
  "ne15:14.1_4": "natiyā asati āgatigati na hoti,",
  "ne15:14.1_5": "āgatigatiyā asati cutūpapāto na hoti,",
  "ne15:14.1_6": "cutūpapāte asati nevidha na huraṁ na ubhayamantarena esevanto dukkhassā”ti.",
  "ne15:15.1_0": "“Nissitassa calitan”ti nissayo nāma duvidho taṇhānissayo ca diṭṭhinissayo ca.",
  "ne15:15.2_0": "Tattha yā rattassa cetanā, ayaṁ taṇhānissayo;",
  "ne15:15.3_0": "yā mūḷhassa cetanā, ayaṁ diṭṭhinissayo.",
  "ne15:15.4_0": "Cetanā pana saṅkhārā,",
  "ne15:15.4_1": "saṅkhārapaccayā viññāṇaṁ,",
  "ne15:15.4_2": "viññāṇapaccayā nāmarūpaṁ,",
  "ne15:15.4_3": "evaṁ sabbo paṭiccasamuppādo.",
  "ne15:15.5_0": "Ayaṁ paṭiccasamuppādehi otaraṇā.",
  "ne15:16.1_0": "Tattha yā rattassa vedanā, ayaṁ sukhā vedanā.",
  "ne15:16.2_0": "Yā sammūḷhassa vedanā, ayaṁ adukkhamasukhā vedanā, imā dve vedanā vedanākkhandho.",
  "ne15:16.3_0": "Ayaṁ khandhehi otaraṇā.",
  "ne15:17.1_0": "Tattha sukhā vedanā dve indriyāni sukhindriyaṁ somanassindriyañca,",
  "ne15:17.1_1": "adukkhamasukhā vedanā upekkhindriyaṁ.",
  "ne15:17.2_0": "Ayaṁ indriyehi otaraṇā.",
  "ne15:18.1_0": "Tāniyeva indriyāni saṅkhārapariyāpannāni,",
  "ne15:18.1_1": "ye saṅkhārā sāsavā bhavaṅgā,",
  "ne15:18.1_2": "te saṅkhārā dhammadhātusaṅgahitā.",
  "ne15:18.2_0": "Ayaṁ dhātūhi otaraṇā.",
  "ne15:19.1_0": "Sā dhammadhātu dhammāyatanapariyāpannā, yaṁ āyatanaṁ sāsavaṁ bhavaṅgaṁ, ayaṁ āyatanehi otaraṇā.",
  "ne15:20.1_0": "“Anissitassa calitaṁ natthī”ti samathavasena vā taṇhāya anissito vipassanāvasena vā diṭṭhiyā anissito.",
  "ne15:20.2_0": "Yā vipassanā ayaṁ vijjā,",
  "ne15:20.2_1": "vijjuppādā avijjānirodho,",
  "ne15:20.2_2": "avijjānirodhā saṅkhāranirodho,",
  "ne15:20.2_3": "saṅkhāranirodhā viññāṇanirodho,",
  "ne15:20.2_4": "evaṁ sabbo paṭiccasamuppādo.",
  "ne15:20.3_0": "Ayaṁ paṭiccasamuppādehi otaraṇā.",
  "ne15:21.1_0": "Sāyeva vipassanā paññākkhandho.",
  "ne15:21.2_0": "Ayaṁ khandhehi otaraṇā.",
  "ne15:22.1_0": "Sāyeva vipassanā dve indriyāni—",
  "ne15:22.2_0": "vīriyindriyañca paññindriyañca.",
  "ne15:22.3_0": "Ayaṁ indriyehi otaraṇā.",
  "ne15:23.1_0": "Sāyeva vipassanā saṅkhārapariyāpannā,",
  "ne15:23.1_1": "ye saṅkhārā anāsavā,",
  "ne15:23.1_2": "no ca bhavaṅgā,",
  "ne15:23.1_3": "te saṅkhārā dhammadhātusaṅgahitā.",
  "ne15:23.2_0": "Ayaṁ dhātūhi otaraṇā.",
  "ne15:24.1_0": "Sā dhammadhātu dhammāyatanapariyāpannā, yaṁ āyatanaṁ anāsavaṁ, no ca bhavaṅgaṁ.",
  "ne15:24.2_0": "Ayaṁ āyatanehi otaraṇā.",
  "ne15:25.1_0": "“Passaddhiyā satī”ti duvidhā passaddhi kāyikā ca cetasikā ca.",
  "ne15:25.2_0": "Yaṁ kāyikaṁ sukhaṁ, ayaṁ kāyapassaddhi.",
  "ne15:25.3_0": "Yaṁ cetasikaṁ sukhaṁ, ayaṁ cetasikā passaddhi.",
  "ne15:25.4_0": "Passaddhakāyo sukhaṁ vediyati,",
  "ne15:25.4_1": "sukhino cittaṁ samādhiyati,",
  "ne15:25.4_2": "samāhito yathābhūtaṁ pajānāti,",
  "ne15:25.4_3": "yathābhūtaṁ pajānanto nibbindati,",
  "ne15:25.4_4": "nibbindanto virajjati,",
  "ne15:25.4_5": "virāgā vimuccati,",
  "ne15:25.4_6": "vimuttasmiṁ “vimuttam”iti ñāṇaṁ hoti,",
  "ne15:25.4_7": "“khīṇā jāti,",
  "ne15:25.4_8": "vusitaṁ brahmacariyaṁ,",
  "ne15:25.4_9": "kataṁ karaṇīyaṁ,",
  "ne15:25.4_10": "nāparaṁ itthattāyā”ti pajānāti.",
  "ne15:25.5_0": "So na namati rūpesu,",
  "ne15:25.5_1": "na saddesu,",
  "ne15:25.5_2": "na gandhesu,",
  "ne15:25.5_3": "na rasesu,",
  "ne15:25.5_4": "na phoṭṭhabbesu,",
  "ne15:25.5_5": "na dhammesu khayā rāgassa khayā dosassa khayā mohassa yena rūpena tathāgataṁ tiṭṭhantaṁ carantaṁ paññāpayamāno paññāpeyya,",
  "ne15:25.5_6": "tassa rūpassa khayā virāgā nirodhā cāgā paṭinissaggā rūpasaṅkhaye vimutto,",
  "ne15:25.5_7": "tathāgato atthītipi na upeti,",
  "ne15:25.5_8": "natthītipi na upeti,",
  "ne15:25.5_9": "atthi natthītipi na upeti,",
  "ne15:25.5_10": "nevatthi no natthītipi na upeti.",
  "ne15:25.6_0": "Atha kho gambhīro appameyyo asaṅkheyyo nibbutotiyeva saṅkhaṁ gacchati khayā rāgassa,",
  "ne15:25.6_1": "khayā dosassa,",
  "ne15:25.6_2": "khayā mohassa.",
  "ne15:26.1_0": "Yāya vedanāya …pe…",
  "ne15:26.2_0": "yāya saññāya.",
  "ne15:26.3_0": "Yehi saṅkhārehi.",
  "ne15:26.4_0": "Yena viññāṇena tathāgataṁ tiṭṭhantaṁ carantaṁ paññāpayamāno paññāpeyya,",
  "ne15:26.4_1": "tassa viññāṇassa khayā virāgā nirodhā cāgā paṭinissaggā viññāṇasaṅkhaye vimutto,",
  "ne15:26.4_2": "tathāgato atthītipi na upeti,",
  "ne15:26.4_3": "natthītipi na upeti,",
  "ne15:26.4_4": "atthi natthītipi na upeti,",
  "ne15:26.4_5": "nevatthi no natthītipi na upeti.",
  "ne15:26.5_0": "Atha kho gambhīro appameyyo asaṅkheyyo nibbutotiyeva saṅkhaṁ gacchati khayā rāgassa,",
  "ne15:26.5_1": "khayā dosassa,",
  "ne15:26.5_2": "khayā mohassa.",
  "ne15:26.6_0": "“Āgatī”ti idhāgati.",
  "ne15:26.7_0": "“Gatī”ti peccabhavo.",
  "ne15:26.8_0": "Āgatigatīpi na bhavanti, “nevidhā”ti chasu ajjhattikesu āyatanesu.",
  "ne15:26.9_0": "“Na huran”ti chasu bāhiresu āyatanesu.",
  "ne15:26.10_0": "“Na ubhayamantarenā”ti phassasamuditesu dhammesu attānaṁ na passati.",
  "ne15:26.11_0": "“Esevanto dukkhassā”ti paṭiccasamuppādo.",
  "ne15:26.12_0": "So duvidho lokiyo ca lokuttaro ca.",
  "ne15:26.13_0": "Tattha lokiyo avijjāpaccayā saṅkhārā, yāva jarāmaraṇā.",
  "ne15:26.14_0": "Lokuttaro sīlavato avippaṭisāro jāyati, yāva nāparaṁ itthattāyāti pajānāti.",
  "ne15:26.15_0": "Tenāha bhagavā “nissitassa calitaṁ anissitassa calitaṁ natthi …pe…",
  "ne15:26.16_0": "esevanto dukkhassā”ti.",
  "ne15:27.1_0": "“Ye keci sokā paridevitā vā,",
  "ne15:27.2_0": "Dukkhā ca lokasmimanekarūpā;",
  "ne15:27.3_0": "Piyaṁ paṭiccappabhavanti ete,",
  "ne15:27.4_0": "Piye asante na bhavanti ete.",
  "ne15:28.1_0": "Tasmā hi te sukhino vītasokā,",
  "ne15:28.2_0": "Yesaṁ piyaṁ natthi kuhiñci loke;",
  "ne15:28.3_0": "Tasmā asokaṁ virajaṁ patthayāno,",
  "ne15:28.4_0": "Piyaṁ na kayirātha kuhiñci loke”ti.",
  "ne15:29.1_0": "“Ye keci sokā paridevitā vā, dukkhā ca lokasmimanekarūpā piyaṁ paṭiccappabhavanti ete”ti—",
  "ne15:29.2_0": "ayaṁ dukkhā vedanā.",
  "ne15:29.3_0": "“Piye asante na bhavanti ete”ti—",
  "ne15:29.4_0": "ayaṁ sukhā vedanā.",
  "ne15:29.5_0": "Vedanā vedanākkhandho.",
  "ne15:29.6_0": "Ayaṁ khandhehi otaraṇā.",
  "ne15:30.1_0": "Vedanāpaccayā taṇhā,",
  "ne15:30.1_1": "taṇhāpaccayā upādānaṁ,",
  "ne15:30.1_2": "upādānapaccayā bhavo,",
  "ne15:30.1_3": "bhavapaccayā jāti,",
  "ne15:30.1_4": "jātipaccayā jarāmaraṇaṁ,",
  "ne15:30.1_5": "evaṁ sabbaṁ.",
  "ne15:30.2_0": "Ayaṁ paṭiccasamuppādehi otaraṇā.",
  "ne15:31.1_0": "Tattha sukhā vedanā dve indriyāni—",
  "ne15:31.2_0": "sukhindriyaṁ somanassindriyañca.",
  "ne15:31.3_0": "Dukkhā vedanā dve indriyāni—",
  "ne15:31.4_0": "dukkhindriyaṁ domanassindriyañca.",
  "ne15:31.5_0": "Ayaṁ indriyehi otaraṇā.",
  "ne15:32.1_0": "Tāniyeva indriyāni saṅkhārapariyāpannāni,",
  "ne15:32.1_1": "ye saṅkhārā sāsavā bhavaṅgā,",
  "ne15:32.1_2": "te saṅkhārā dhammadhātusaṅgahitā.",
  "ne15:32.2_0": "Ayaṁ dhātūhi otaraṇā.",
  "ne15:33.1_0": "Sā dhammadhātu dhammāyatanapariyāpannā, yaṁ āyatanaṁ sāsavaṁ bhavaṅgaṁ.",
  "ne15:33.2_0": "Ayaṁ āyatanehi otaraṇā.",
  "ne15:34.1_0": "“Tasmā hi te sukhino vītasokā,",
  "ne15:34.2_0": "Yesaṁ piyaṁ natthi kuhiñci loke;",
  "ne15:34.3_0": "Tasmā asokaṁ virajaṁ patthayāno,",
  "ne15:34.4_0": "Piyaṁ na kayirātha kuhiñci loke”ti.",
  "ne15:35.1_0": "Idaṁ taṇhāpahānaṁ.",
  "ne15:35.2_0": "Taṇhānirodhā upādānanirodho, upādānanirodhā bhavanirodho, evaṁ sabbaṁ.",
  "ne15:35.3_0": "Ayaṁ paṭiccasamuppādehi otaraṇā.",
  "ne15:36.1_0": "Taṁyeva taṇhāpahānaṁ samatho.",
  "ne15:36.2_0": "So samatho dve indriyāni satindriyaṁ samādhindriyañca.",
  "ne15:36.3_0": "Ayaṁ indriyehi otaraṇā.",
  "ne15:37.1_0": "Soyeva samatho samādhikkhandho.",
  "ne15:37.2_0": "Ayaṁ khandhehi otaraṇā.",
  "ne15:38.1_0": "Soyeva samatho saṅkhārapariyāpanno,",
  "ne15:38.1_1": "ye saṅkhārā anāsavā,",
  "ne15:38.1_2": "no ca bhavaṅgā,",
  "ne15:38.1_3": "te saṅkhārā dhammadhātusaṅgahitā.",
  "ne15:38.2_0": "Ayaṁ dhātūhi otaraṇā.",
  "ne15:39.1_0": "Sā dhammadhātu dhammāyatanapariyāpannā, yaṁ āyatanaṁ anāsavaṁ, no ca bhavaṅgaṁ.",
  "ne15:39.2_0": "Ayaṁ āyatanehi otaraṇā.",
  "ne15:39.3_0": "Tenāha bhagavā “ye keci sokā”ti.",
  "ne15:40.1_0": "“Kāmaṁ kāmayamānassa,",
  "ne15:40.2_0": "tassa ce taṁ samijjhati;",
  "ne15:40.3_0": "Addhā pītimano hoti,",
  "ne15:40.4_0": "laddhā macco yadicchati.",
  "ne15:41.1_0": "Tassa ce kāmayānassa,",
  "ne15:41.2_0": "chandajātassa jantuno;",
  "ne15:41.3_0": "Te kāmā parihāyanti,",
  "ne15:41.4_0": "sallaviddhova ruppati.",
  "ne15:42.1_0": "Yo kāme parivajjeti,",
  "ne15:42.2_0": "sappasseva padā siro;",
  "ne15:42.3_0": "Somaṁ visattikaṁ loke,",
  "ne15:42.4_0": "sato samativattatī”ti.",
  "ne15:43.1_0": "Tattha yā pītimanatā, ayaṁ anunayo.",
  "ne15:43.2_0": "Yadāha sallaviddhova ruppatīti, idaṁ paṭighaṁ.",
  "ne15:43.3_0": "Anunayaṁ paṭighañca pana taṇhāpakkho, taṇhāya ca pana dasarūpīni āyatanāni padaṭṭhānaṁ.",
  "ne15:43.4_0": "Ayaṁ āyatanehi otaraṇā.",
  "ne15:44.1_0": "Tāniyeva dasa rūpīni rūpakāyo nāmasampayutto,",
  "ne15:44.1_1": "tadubhayaṁ nāmarūpaṁ,",
  "ne15:44.1_2": "nāmarūpapaccayā saḷāyatanaṁ,",
  "ne15:44.1_3": "saḷāyatanapaccayā phasso,",
  "ne15:44.1_4": "phassapaccayā vedanā,",
  "ne15:44.1_5": "vedanāpaccayā taṇhā,",
  "ne15:44.1_6": "evaṁ sabbaṁ.",
  "ne15:44.2_0": "Ayaṁ paṭiccasamuppādehi otaraṇā.",
  "ne15:45.1_0": "Tadeva nāmarūpaṁ pañcakkhandho.",
  "ne15:45.2_0": "Ayaṁ khandhehi otaraṇā.",
  "ne15:46.1_0": "Tadeva nāmarūpaṁ aṭṭhārasa dhātuyo.",
  "ne15:46.2_0": "Ayaṁ dhātūhi otaraṇā.",
  "ne15:47.1_0": "Tattha yo rūpakāyo imāni pañca rūpīni indriyāni,",
  "ne15:47.1_1": "yo nāmakāyo imāni pañca arūpīni indriyāni,",
  "ne15:47.1_2": "imāni dasa indriyāni.",
  "ne15:47.2_0": "Ayaṁ indriyehi otaraṇā.",
  "ne15:48.1_0": "Tattha yadāha—",
  "ne15:49.1_0": "“Yo kāme parivajjeti,",
  "ne15:49.2_0": "sappasseva padā siro;",
  "ne15:49.3_0": "Somaṁ visattikaṁ loke,",
  "ne15:49.4_0": "sato samativattatī”ti.",
  "ne15:50.1_0": "Ayaṁ saupādisesā nibbānadhātu, ayaṁ dhātūhi otaraṇā.",
  "ne15:51.1_0": "Sāyeva saupādisesā nibbānadhātu vijjā,",
  "ne15:51.1_1": "vijjuppādā avijjānirodho,",
  "ne15:51.1_2": "avijjānirodhā saṅkhāranirodho,",
  "ne15:51.1_3": "evaṁ sabbaṁ.",
  "ne15:51.2_0": "Ayaṁ paṭiccasamuppādehi otaraṇā.",
  "ne15:52.1_0": "Sāyeva vijjā paññākkhandho.",
  "ne15:52.2_0": "Ayaṁ khandhehi otaraṇā.",
  "ne15:53.1_0": "Sāyeva vijjā dve indriyāni—",
  "ne15:53.2_0": "vīriyindriyaṁ paññindriyañca.",
  "ne15:53.3_0": "Ayaṁ indriyehi otaraṇā.",
  "ne15:54.1_0": "Sāyeva vijjā saṅkhārapariyāpannā,",
  "ne15:54.1_1": "ye saṅkhārā anāsavā,",
  "ne15:54.1_2": "no ca bhavaṅgā,",
  "ne15:54.1_3": "te saṅkhārā dhammadhātusaṅgahitā.",
  "ne15:54.2_0": "Ayaṁ dhātūhi otaraṇā.",
  "ne15:55.1_0": "Sā dhammadhātu dhammāyatanapariyāpannā, yaṁ āyatanaṁ anāsavaṁ, no ca bhavaṅgaṁ.",
  "ne15:55.2_0": "Ayaṁ āyatanehi otaraṇā.",
  "ne15:55.3_0": "Tenāha bhagavā “kāmaṁ kāmayamānassā”ti.",
  "ne15:56.1_0": "Ettāvatā paṭicca indriyakhandhadhātuāyatanāni samosaraṇotaraṇāni bhavanti.",
  "ne15:56.2_0": "Evaṁ paṭicca indriyakhandhadhātuāyatanāni otāretabbāni.",
  "ne15:56.3_0": "Tenāha āyasmā mahākaccāyano “yo ca paṭiccuppādo”ti.",
  "ne15:56.4_0": "Niyutto otaraṇo hāro."
}