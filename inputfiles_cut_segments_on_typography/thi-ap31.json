{
  "thi-ap31:0.1_0": "Therīapadāna",
  "thi-ap31:0.2_0": "Khattiyāvagga",
  "thi-ap31:0.3_0": "1 Yasavatīpamukhaaṭṭhārasabhikkhunīsahassaapadāna",
  "thi-ap31:1.1_0": "“Bhavā sabbe parikkhīṇā,",
  "thi-ap31:1.2_0": "bhavā santi vimocitā;",
  "thi-ap31:1.3_0": "Sabbāsavā ca no natthi,",
  "thi-ap31:1.4_0": "ārocema mahāmune.",
  "thi-ap31:2.1_0": "Purimaṁ kusalaṁ kammaṁ,",
  "thi-ap31:2.2_0": "yaṁ kiñci sādhupatthitaṁ;",
  "thi-ap31:2.3_0": "Paribhogamayaṁ dinnaṁ,",
  "thi-ap31:2.4_0": "tuyhatthāya mahāmune.",
  "thi-ap31:3.1_0": "Buddhapaccekabuddhānaṁ,",
  "thi-ap31:3.2_0": "sāvakānañca patthitaṁ;",
  "thi-ap31:3.3_0": "Paribhogamayaṁ dinnaṁ,",
  "thi-ap31:3.4_0": "tuyhatthāya mahāmune.",
  "thi-ap31:4.1_0": "Uccanīcamayaṁ kammaṁ,",
  "thi-ap31:4.2_0": "bhikkhūnaṁ sādhupatthitaṁ;",
  "thi-ap31:4.3_0": "Uccākulaparikammaṁ,",
  "thi-ap31:4.4_0": "katametaṁ mahāmune.",
  "thi-ap31:5.1_0": "Teneva sukkamūlena,",
  "thi-ap31:5.2_0": "coditā kammasampadā;",
  "thi-ap31:5.3_0": "Mānusikamatikkantā,",
  "thi-ap31:5.4_0": "jāyiṁsu khattiye kule.",
  "thi-ap31:6.1_0": "Uppatte ca kate kamme,",
  "thi-ap31:6.2_0": "jātiyā vāpi ekato;",
  "thi-ap31:6.3_0": "Pacchime ekato jātā,",
  "thi-ap31:6.4_0": "khattiyā kulasambhavā.",
  "thi-ap31:7.1_0": "Rūpavatī bhogavatī,",
  "thi-ap31:7.2_0": "lābhasakkārapūjitā;",
  "thi-ap31:7.3_0": "Antepure mahāvīra,",
  "thi-ap31:7.4_0": "devānaṁ viya nandane.",
  "thi-ap31:8.1_0": "Nibbinditvā agāramhā,",
  "thi-ap31:8.2_0": "pabbajimhanagāriyaṁ;",
  "thi-ap31:8.3_0": "Katipāhaṁ upādāya,",
  "thi-ap31:8.4_0": "sabbā pattāmha nibbutiṁ.",
  "thi-ap31:9.1_0": "Cīvaraṁ piṇḍapātañca,",
  "thi-ap31:9.2_0": "paccayaṁ sayanāsanaṁ;",
  "thi-ap31:9.3_0": "Upanenti bahū amhe,",
  "thi-ap31:9.4_0": "sadā sakkatapūjitā.",
  "thi-ap31:10.1_0": "Kilesā jhāpitā amhaṁ,",
  "thi-ap31:10.2_0": "bhavā sabbe samūhatā;",
  "thi-ap31:10.3_0": "Nāgīva bandhanaṁ chetvā,",
  "thi-ap31:10.4_0": "viharāma anāsavā.",
  "thi-ap31:11.1_0": "Svāgataṁ vata no āsi,",
  "thi-ap31:11.2_0": "mama buddhassa santike;",
  "thi-ap31:11.3_0": "Tisso vijjā anuppattā,",
  "thi-ap31:11.4_0": "kataṁ buddhassa sāsanaṁ.",
  "thi-ap31:12.1_0": "Paṭisambhidā catasso,",
  "thi-ap31:12.2_0": "vimokkhāpi ca aṭṭhime;",
  "thi-ap31:12.3_0": "Chaḷabhiññā sacchikatā,",
  "thi-ap31:12.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "thi-ap31:13.1_0": "Itthaṁ sudaṁ yasavatīpamukhāni khattiyakaññābhikkhuniyo aṭṭhārasasahassāni bhagavato sammukhā imā gāthāyo abhāsitthāti.",
  "thi-ap31:14.1_0": "Yasavatīpamukhaaṭṭhārasabhikkhunīsahassāpadānaṁ paṭhamaṁ."
}