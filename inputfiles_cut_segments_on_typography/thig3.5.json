{
  "thig3.5:0.1_0": "Therīgāthā 3.5",
  "thig3.5:0.2_0": "Tikanipāta",
  "thig3.5:0.3_0": "Ubbiritherīgāthā",
  "thig3.5:1.1_0": "“Amma jīvāti vanamhi kandasi,",
  "thig3.5:1.2_0": "Attānaṁ adhigaccha ubbiri;",
  "thig3.5:1.3_0": "Cullāsītisahassāni,",
  "thig3.5:1.4_0": "Sabbā jīvasanāmikā;",
  "thig3.5:1.5_0": "Etamhāḷāhane daḍḍhā,",
  "thig3.5:1.6_0": "Tāsaṁ kamanusocasi”.",
  "thig3.5:2.1_0": "“Abbahī vata me sallaṁ,",
  "thig3.5:2.2_0": "duddasaṁ hadayassitaṁ;",
  "thig3.5:2.3_0": "Yaṁ me sokaparetāya,",
  "thig3.5:2.4_0": "dhītusokaṁ byapānudi.",
  "thig3.5:3.1_0": "Sājja abbūḷhasallāhaṁ,",
  "thig3.5:3.2_0": "Nicchātā parinibbutā;",
  "thig3.5:3.3_0": "Buddhaṁ dhammañca saṅghañca,",
  "thig3.5:3.4_0": "Upemi saraṇaṁ munin”ti.",
  "thig3.5:4.1_0": "… Ubbirī therī …."
}