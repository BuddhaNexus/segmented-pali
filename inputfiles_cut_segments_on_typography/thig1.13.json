{
  "thig1.13:0.1_0": "Therīgāthā 1.13",
  "thig1.13:0.2_0": "Ekakanipāta",
  "thig1.13:0.3_0": "Visākhātherīgāthā",
  "thig1.13:1.1_0": "“Karotha buddhasāsanaṁ,",
  "thig1.13:1.2_0": "yaṁ katvā nānutappati;",
  "thig1.13:1.3_0": "Khippaṁ pādāni dhovitvā,",
  "thig1.13:1.4_0": "ekamante nisīdathā”ti.",
  "thig1.13:2.1_0": "… Visākhā therī …."
}