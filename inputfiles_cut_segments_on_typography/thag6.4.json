{
  "thag6.4:0.1_0": "Theragāthā 6.4",
  "thag6.4:0.2_0": "Chakkanipāta",
  "thag6.4:0.3_0": "Paṭhamavagga",
  "thag6.4:0.4_0": "Kullattheragāthā",
  "thag6.4:1.1_0": "“Kullo sivathikaṁ gantvā,",
  "thag6.4:1.2_0": "addasa itthimujjhitaṁ;",
  "thag6.4:1.3_0": "Apaviddhaṁ susānasmiṁ,",
  "thag6.4:1.4_0": "khajjantiṁ kimihī phuṭaṁ.",
  "thag6.4:2.1_0": "Āturaṁ asuciṁ pūtiṁ,",
  "thag6.4:2.2_0": "passa kulla samussayaṁ;",
  "thag6.4:2.3_0": "Uggharantaṁ paggharantaṁ,",
  "thag6.4:2.4_0": "bālānaṁ abhinanditaṁ.",
  "thag6.4:3.1_0": "Dhammādāsaṁ gahetvāna,",
  "thag6.4:3.2_0": "ñāṇadassanapattiyā;",
  "thag6.4:3.3_0": "Paccavekkhiṁ imaṁ kāyaṁ,",
  "thag6.4:3.4_0": "tucchaṁ santarabāhiraṁ.",
  "thag6.4:4.1_0": "Yathā idaṁ tathā etaṁ,",
  "thag6.4:4.2_0": "yathā etaṁ tathā idaṁ;",
  "thag6.4:4.3_0": "Yathā adho tathā uddhaṁ,",
  "thag6.4:4.4_0": "yathā uddhaṁ tathā adho.",
  "thag6.4:5.1_0": "Yathā divā tathā rattiṁ,",
  "thag6.4:5.2_0": "yathā rattiṁ tathā divā;",
  "thag6.4:5.3_0": "Yathā pure tathā pacchā,",
  "thag6.4:5.4_0": "yathā pacchā tathā pure.",
  "thag6.4:6.1_0": "Pañcaṅgikena turiyena,",
  "thag6.4:6.2_0": "Na ratī hoti tādisī;",
  "thag6.4:6.3_0": "Yathā ekaggacittassa,",
  "thag6.4:6.4_0": "Sammā dhammaṁ vipassato”ti.",
  "thag6.4:6.5_0": "… Kullo thero …."
}