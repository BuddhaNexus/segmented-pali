{
  "snp5.9:0.1_0": "Sutta Nipāta 5.9",
  "snp5.9:0.2_0": "Hemakamāṇavapucchā",
  "snp5.9:1.1_0": "“Ye me pubbe viyākaṁsu,",
  "snp5.9:1.2_0": "(iccāyasmā hemako)",
  "snp5.9:1.3_0": "Huraṁ gotamasāsanā;",
  "snp5.9:1.4_0": "Iccāsi iti bhavissati,",
  "snp5.9:1.5_0": "Sabbaṁ taṁ itihītihaṁ;",
  "snp5.9:1.6_0": "Sabbaṁ taṁ takkavaḍḍhanaṁ,",
  "snp5.9:1.7_0": "Nāhaṁ tattha abhiramiṁ.",
  "snp5.9:2.1_0": "Tvañca me dhammamakkhāhi,",
  "snp5.9:2.2_0": "taṇhānigghātanaṁ muni;",
  "snp5.9:2.3_0": "Yaṁ viditvā sato caraṁ,",
  "snp5.9:2.4_0": "tare loke visattikaṁ”.",
  "snp5.9:3.1_0": "“Idha diṭṭhasutamutaviññātesu,",
  "snp5.9:3.2_0": "Piyarūpesu hemaka;",
  "snp5.9:3.3_0": "Chandarāgavinodanaṁ,",
  "snp5.9:3.4_0": "Nibbānapadamaccutaṁ.",
  "snp5.9:4.1_0": "Etadaññāya ye satā,",
  "snp5.9:4.2_0": "Diṭṭhadhammābhinibbutā;",
  "snp5.9:4.3_0": "Upasantā ca te sadā,",
  "snp5.9:4.4_0": "Tiṇṇā loke visattikan”ti.",
  "snp5.9:4.5_0": "Hemakamāṇavapucchā aṭṭhamā."
}