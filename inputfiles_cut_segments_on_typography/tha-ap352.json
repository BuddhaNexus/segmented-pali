{
  "tha-ap352:0.1_0": "Therāpadāna",
  "tha-ap352:0.2_0": "Ekapadumiyavagga",
  "tha-ap352:0.3_0": "10. Ekañjaliyattheraapadāna",
  "tha-ap352:1.1_0": "“Romaso nāma sambuddho,",
  "tha-ap352:1.2_0": "nadīkūle vasī tadā;",
  "tha-ap352:1.3_0": "Addasaṁ virajaṁ buddhaṁ,",
  "tha-ap352:1.4_0": "pītaraṁsiṁva bhāṇumaṁ.",
  "tha-ap352:2.1_0": "Ukkāmukhapahaṭṭhaṁva,",
  "tha-ap352:2.2_0": "khadiraṅgārasannibhaṁ;",
  "tha-ap352:2.3_0": "Osadhiṁva virocantaṁ,",
  "tha-ap352:2.4_0": "ekañjalimakāsahaṁ.",
  "tha-ap352:3.1_0": "Catunnavutito kappe,",
  "tha-ap352:3.2_0": "yaṁ añjalimakāsahaṁ;",
  "tha-ap352:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap352:3.4_0": "añjaliyā idaṁ phalaṁ.",
  "tha-ap352:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap352:4.2_0": "…pe…",
  "tha-ap352:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap352:5.1_0": "Itthaṁ sudaṁ āyasmā ekañjaliyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap352:6.1_0": "Ekañjaliyattherassāpadānaṁ dasamaṁ.",
  "tha-ap352:7.1_0": "Ekapadumiyavaggo pañcatiṁsatimo.",
  "tha-ap352:8.1_0": "Tassuddānaṁ",
  "tha-ap352:9.1_0": "Padumī uppalamālī,",
  "tha-ap352:9.2_0": "dhajo kiṅkaṇikaṁ naḷaṁ;",
  "tha-ap352:9.3_0": "Campako padumo muṭṭhi,",
  "tha-ap352:9.4_0": "tindukekañjalī tathā;",
  "tha-ap352:9.5_0": "Cha ca saṭṭhi ca gāthāyo,",
  "tha-ap352:9.6_0": "gaṇitāyo vibhāvibhi."
}