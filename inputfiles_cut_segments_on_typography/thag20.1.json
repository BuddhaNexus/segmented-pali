{
  "thag20.1:0.1_0": "Theragāthā 20.1",
  "thag20.1:0.2_0": "Saṭṭhinipāta",
  "thag20.1:0.3_0": "Paṭhamavagga",
  "thag20.1:0.4_0": "Mahāmoggallānattheragāthā",
  "thag20.1:1.1_0": "“Āraññikā piṇḍapātikā,",
  "thag20.1:1.2_0": "uñchāpattāgate ratā;",
  "thag20.1:1.3_0": "Dālemu maccuno senaṁ,",
  "thag20.1:1.4_0": "ajjhattaṁ susamāhitā.",
  "thag20.1:2.1_0": "Āraññikā piṇḍapātikā,",
  "thag20.1:2.2_0": "uñchāpattāgate ratā;",
  "thag20.1:2.3_0": "Dhunāma maccuno senaṁ,",
  "thag20.1:2.4_0": "naḷāgāraṁva kuñjaro.",
  "thag20.1:3.1_0": "Rukkhamūlikā sātatikā,",
  "thag20.1:3.2_0": "uñchāpattāgate ratā;",
  "thag20.1:3.3_0": "Dālemu maccuno senaṁ,",
  "thag20.1:3.4_0": "ajjhattaṁ susamāhitā.",
  "thag20.1:4.1_0": "Rukkhamūlikā sātatikā,",
  "thag20.1:4.2_0": "uñchāpattāgate ratā;",
  "thag20.1:4.3_0": "Dhunāma maccuno senaṁ,",
  "thag20.1:4.4_0": "naḷāgāraṁva kuñjaro”.",
  "thag20.1:5.1_0": "“Aṭṭhikaṅkalakuṭike,",
  "thag20.1:5.2_0": "maṁsanhārupasibbite;",
  "thag20.1:5.3_0": "Dhiratthu pure duggandhe,",
  "thag20.1:5.4_0": "paragatte mamāyase.",
  "thag20.1:6.1_0": "Gūthabhaste taconaddhe,",
  "thag20.1:6.2_0": "uragaṇḍipisācini;",
  "thag20.1:6.3_0": "Nava sotāni te kāye,",
  "thag20.1:6.4_0": "yāni sandanti sabbadā.",
  "thag20.1:7.1_0": "Tava sarīraṁ navasotaṁ,",
  "thag20.1:7.2_0": "Duggandhakaraṁ paribandhaṁ;",
  "thag20.1:7.3_0": "Bhikkhu parivajjayate taṁ,",
  "thag20.1:7.4_0": "Mīḷhaṁ ca yathā sucikāmo.",
  "thag20.1:8.1_0": "Evañce taṁ jano jaññā,",
  "thag20.1:8.2_0": "yathā jānāmi taṁ ahaṁ;",
  "thag20.1:8.3_0": "Ārakā parivajjeyya,",
  "thag20.1:8.4_0": "gūthaṭṭhānaṁva pāvuse”.",
  "thag20.1:9.1_0": "“Evametaṁ mahāvīra,",
  "thag20.1:9.2_0": "yathā samaṇa bhāsasi;",
  "thag20.1:9.3_0": "Ettha ceke visīdanti,",
  "thag20.1:9.4_0": "paṅkamhiva jaraggavo”.",
  "thag20.1:10.1_0": "“Ākāsamhi haliddiyā,",
  "thag20.1:10.2_0": "yo maññetha rajetave;",
  "thag20.1:10.3_0": "Aññena vāpi raṅgena,",
  "thag20.1:10.4_0": "vighātudayameva taṁ.",
  "thag20.1:11.1_0": "Tadākāsasamaṁ cittaṁ,",
  "thag20.1:11.2_0": "ajjhattaṁ susamāhitaṁ;",
  "thag20.1:11.3_0": "Mā pāpacitte āsādi,",
  "thag20.1:11.4_0": "aggikhandhaṁva pakkhimā”.",
  "thag20.1:12.1_0": "“Passa cittakataṁ bimbaṁ,",
  "thag20.1:12.2_0": "arukāyaṁ samussitaṁ;",
  "thag20.1:12.3_0": "Āturaṁ bahusaṅkappaṁ,",
  "thag20.1:12.4_0": "yassa natthi dhuvaṁ ṭhiti.",
  "thag20.1:13.1_0": "Passa cittakataṁ rūpaṁ,",
  "thag20.1:13.2_0": "maṇinā kuṇḍalena ca;",
  "thag20.1:13.3_0": "Aṭṭhiṁ tacena onaddhaṁ,",
  "thag20.1:13.4_0": "saha vatthehi sobhati.",
  "thag20.1:14.1_0": "Alattakakatā pādā,",
  "thag20.1:14.2_0": "mukhaṁ cuṇṇakamakkhitaṁ;",
  "thag20.1:14.3_0": "Alaṁ bālassa mohāya,",
  "thag20.1:14.4_0": "no ca pāragavesino.",
  "thag20.1:15.1_0": "Aṭṭhapadakatā kesā,",
  "thag20.1:15.2_0": "nettā añjanamakkhitā;",
  "thag20.1:15.3_0": "Alaṁ bālassa mohāya,",
  "thag20.1:15.4_0": "no ca pāragavesino.",
  "thag20.1:16.1_0": "Añjanīva navā cittā,",
  "thag20.1:16.2_0": "pūtikāyo alaṅkato;",
  "thag20.1:16.3_0": "Alaṁ bālassa mohāya,",
  "thag20.1:16.4_0": "no ca pāragavesino.",
  "thag20.1:17.1_0": "Odahi migavo pāsaṁ,",
  "thag20.1:17.2_0": "nāsadā vāguraṁ migo;",
  "thag20.1:17.3_0": "Bhutvā nivāpaṁ gacchāma,",
  "thag20.1:17.4_0": "kaddante migabandhake.",
  "thag20.1:18.1_0": "Chinno pāso migavassa,",
  "thag20.1:18.2_0": "nāsadā vāguraṁ migo;",
  "thag20.1:18.3_0": "Bhutvā nivāpaṁ gacchāma,",
  "thag20.1:18.4_0": "socante migaluddake”.",
  "thag20.1:19.1_0": "“Tadāsi yaṁ bhiṁsanakaṁ,",
  "thag20.1:19.2_0": "tadāsi lomahaṁsanaṁ;",
  "thag20.1:19.3_0": "Anekākārasampanne,",
  "thag20.1:19.4_0": "sāriputtamhi nibbute.",
  "thag20.1:20.1_0": "Aniccā vata saṅkhārā,",
  "thag20.1:20.2_0": "uppādavayadhammino;",
  "thag20.1:20.3_0": "Upajjitvā nirujjhanti,",
  "thag20.1:20.4_0": "tesaṁ vūpasamo sukho.",
  "thag20.1:21.1_0": "Sukhumaṁ te paṭivijjhanti,",
  "thag20.1:21.2_0": "vālaggaṁ usunā yathā;",
  "thag20.1:21.3_0": "Ye pañcakkhandhe passanti,",
  "thag20.1:21.4_0": "parato no ca attato.",
  "thag20.1:22.1_0": "Ye ca passanti saṅkhāre,",
  "thag20.1:22.2_0": "parato no ca attato;",
  "thag20.1:22.3_0": "Paccabyādhiṁsu nipuṇaṁ,",
  "thag20.1:22.4_0": "vālaggaṁ usunā yathā.",
  "thag20.1:23.1_0": "Sattiyā viya omaṭṭho,",
  "thag20.1:23.2_0": "ḍayhamānova matthake;",
  "thag20.1:23.3_0": "Kāmarāgappahānāya,",
  "thag20.1:23.4_0": "sato bhikkhu paribbaje.",
  "thag20.1:24.1_0": "Sattiyā viya omaṭṭho,",
  "thag20.1:24.2_0": "ḍayhamānova matthake;",
  "thag20.1:24.3_0": "Bhavarāgappahānāya,",
  "thag20.1:24.4_0": "sato bhikkhu paribbaje.",
  "thag20.1:25.1_0": "Codito bhāvitattena,",
  "thag20.1:25.2_0": "sarīrantimadhārinā;",
  "thag20.1:25.3_0": "Migāramātupāsādaṁ,",
  "thag20.1:25.4_0": "pādaṅguṭṭhena kampayiṁ.",
  "thag20.1:26.1_0": "Nayidaṁ sithilamārabbha,",
  "thag20.1:26.2_0": "nayidaṁ appena thāmasā;",
  "thag20.1:26.3_0": "Nibbānamadhigantabbaṁ,",
  "thag20.1:26.4_0": "sabbaganthapamocanaṁ.",
  "thag20.1:27.1_0": "Ayañca daharo bhikkhu,",
  "thag20.1:27.2_0": "ayamuttamaporiso;",
  "thag20.1:27.3_0": "Dhāreti antimaṁ dehaṁ,",
  "thag20.1:27.4_0": "jetvā māraṁ savāhiniṁ.",
  "thag20.1:28.1_0": "Vivaramanupabhanti vijjutā,",
  "thag20.1:28.2_0": "Vebhārassa ca paṇḍavassa ca;",
  "thag20.1:28.3_0": "Nagavivaragato jhāyati,",
  "thag20.1:28.4_0": "Putto appaṭimassa tādino.",
  "thag20.1:29.1_0": "Upasanto uparato,",
  "thag20.1:29.2_0": "pantasenāsano muni;",
  "thag20.1:29.3_0": "Dāyādo buddhaseṭṭhassa,",
  "thag20.1:29.4_0": "brahmunā abhivandito”.",
  "thag20.1:30.1_0": "“Upasantaṁ uparataṁ,",
  "thag20.1:30.2_0": "pantasenāsanaṁ muniṁ;",
  "thag20.1:30.3_0": "Dāyādaṁ buddhaseṭṭhassa,",
  "thag20.1:30.4_0": "vanda brāhmaṇa kassapaṁ.",
  "thag20.1:31.1_0": "Yo ca jātisataṁ gacche,",
  "thag20.1:31.2_0": "sabbā brāhmaṇajātiyo;",
  "thag20.1:31.3_0": "Sottiyo vedasampanno,",
  "thag20.1:31.4_0": "manussesu punappunaṁ.",
  "thag20.1:32.1_0": "Ajjhāyakopi ce assa,",
  "thag20.1:32.2_0": "tiṇṇaṁ vedāna pāragū;",
  "thag20.1:32.3_0": "Etassa vandanāyetaṁ,",
  "thag20.1:32.4_0": "kalaṁ nāgghati soḷasiṁ.",
  "thag20.1:33.1_0": "Yo so aṭṭha vimokkhāni,",
  "thag20.1:33.2_0": "purebhattaṁ aphassayi;",
  "thag20.1:33.3_0": "Anulomaṁ paṭilomaṁ,",
  "thag20.1:33.4_0": "tato piṇḍāya gacchati.",
  "thag20.1:34.1_0": "Tādisaṁ bhikkhuṁ māsādi,",
  "thag20.1:34.2_0": "māttānaṁ khaṇi brāhmaṇa;",
  "thag20.1:34.3_0": "Abhippasādehi manaṁ,",
  "thag20.1:34.4_0": "arahantamhi tādine;",
  "thag20.1:34.5_0": "Khippaṁ pañjaliko vanda,",
  "thag20.1:34.6_0": "mā te vijaṭi matthakaṁ”.",
  "thag20.1:35.1_0": "“Neso passati saddhammaṁ,",
  "thag20.1:35.2_0": "saṁsārena purakkhato;",
  "thag20.1:35.3_0": "Adhogamaṁ jimhapathaṁ,",
  "thag20.1:35.4_0": "kummaggamanudhāvati.",
  "thag20.1:36.1_0": "Kimīva mīḷhasallitto,",
  "thag20.1:36.2_0": "saṅkhāre adhimucchito;",
  "thag20.1:36.3_0": "Pagāḷho lābhasakkāre,",
  "thag20.1:36.4_0": "tuccho gacchati poṭṭhilo”.",
  "thag20.1:37.1_0": "“Imañca passa āyantaṁ,",
  "thag20.1:37.2_0": "sāriputtaṁ sudassanaṁ;",
  "thag20.1:37.3_0": "Vimuttaṁ ubhatobhāge,",
  "thag20.1:37.4_0": "ajjhattaṁ susamāhitaṁ.",
  "thag20.1:38.1_0": "Visallaṁ khīṇasaṁyogaṁ,",
  "thag20.1:38.2_0": "tevijjaṁ maccuhāyinaṁ;",
  "thag20.1:38.3_0": "Dakkhiṇeyyaṁ manussānaṁ,",
  "thag20.1:38.4_0": "puññakkhettaṁ anuttaraṁ”.",
  "thag20.1:39.1_0": "“Ete sambahulā devā,",
  "thag20.1:39.2_0": "iddhimanto yasassino;",
  "thag20.1:39.3_0": "Dasa devasahassāni,",
  "thag20.1:39.4_0": "sabbe brahmapurohitā;",
  "thag20.1:39.5_0": "Moggallānaṁ namassantā,",
  "thag20.1:39.6_0": "tiṭṭhanti pañjalīkatā.",
  "thag20.1:40.1_0": "‘Namo te purisājañña,",
  "thag20.1:40.2_0": "namo te purisuttama;",
  "thag20.1:40.3_0": "Yassa te āsavā khīṇā,",
  "thag20.1:40.4_0": "dakkhiṇeyyosi mārisa’.",
  "thag20.1:41.1_0": "Pūjito naradevena,",
  "thag20.1:41.2_0": "uppanno maraṇābhibhū;",
  "thag20.1:41.3_0": "Puṇḍarīkaṁva toyena,",
  "thag20.1:41.4_0": "saṅkhārenupalippati.",
  "thag20.1:42.1_0": "Yassa muhuttena sahassadhā loko,",
  "thag20.1:42.2_0": "Saṁvidito sabrahmakappo vasi;",
  "thag20.1:42.3_0": "Iddhiguṇe cutupapāte kāle,",
  "thag20.1:42.4_0": "Passati devatā sa bhikkhu”.",
  "thag20.1:43.1_0": "“Sāriputtova paññāya,",
  "thag20.1:43.2_0": "sīlena upasamena ca;",
  "thag20.1:43.3_0": "Yopi pāraṅgato bhikkhu,",
  "thag20.1:43.4_0": "etāvaparamo siyā.",
  "thag20.1:44.1_0": "Koṭisatasahassassa,",
  "thag20.1:44.2_0": "Attabhāvaṁ khaṇena nimmine;",
  "thag20.1:44.3_0": "Ahaṁ vikubbanāsu kusalo,",
  "thag20.1:44.4_0": "Vasībhūtomhi iddhiyā.",
  "thag20.1:45.1_0": "Samādhivijjāvasipāramīgato,",
  "thag20.1:45.2_0": "Moggallānagotto asitassa sāsane;",
  "thag20.1:45.3_0": "Dhīro samucchindi samāhitindriyo,",
  "thag20.1:45.4_0": "Nāgo yathā pūtilataṁva bandhanaṁ.",
  "thag20.1:46.1_0": "Pariciṇṇo mayā satthā,",
  "thag20.1:46.2_0": "kataṁ buddhassa sāsanaṁ;",
  "thag20.1:46.3_0": "Ohito garuko bhāro,",
  "thag20.1:46.4_0": "bhavanetti samūhatā.",
  "thag20.1:47.1_0": "Yassa catthāya pabbajito,",
  "thag20.1:47.2_0": "agārasmānagāriyaṁ;",
  "thag20.1:47.3_0": "So me attho anuppatto,",
  "thag20.1:47.4_0": "sabbasaṁyojanakkhayo.",
  "thag20.1:48.1_0": "Kīdiso nirayo āsi,",
  "thag20.1:48.2_0": "yattha dussī apaccatha;",
  "thag20.1:48.3_0": "Vidhuraṁ sāvakamāsajja,",
  "thag20.1:48.4_0": "kakusandhañca brāhmaṇaṁ.",
  "thag20.1:49.1_0": "Sataṁ āsi ayosaṅkū,",
  "thag20.1:49.2_0": "sabbe paccattavedanā;",
  "thag20.1:49.3_0": "Īdiso nirayo āsi,",
  "thag20.1:49.4_0": "yattha dussī apaccatha;",
  "thag20.1:49.5_0": "Vidhuraṁ sāvakamāsajja,",
  "thag20.1:49.6_0": "kakusandhañca brāhmaṇaṁ.",
  "thag20.1:50.1_0": "Yo etamabhijānāti,",
  "thag20.1:50.2_0": "bhikkhu buddhassa sāvako;",
  "thag20.1:50.3_0": "Tādisaṁ bhikkhumāsajja,",
  "thag20.1:50.4_0": "kaṇha dukkhaṁ nigacchasi.",
  "thag20.1:51.1_0": "Majjhesarasmiṁ tiṭṭhanti,",
  "thag20.1:51.2_0": "vimānā kappaṭhāyino;",
  "thag20.1:51.3_0": "Veḷuriyavaṇṇā rucirā,",
  "thag20.1:51.4_0": "accimanto pabhassarā;",
  "thag20.1:51.5_0": "Accharā tattha naccanti,",
  "thag20.1:51.6_0": "puthu nānattavaṇṇiyo.",
  "thag20.1:52.1_0": "Yo etamabhijānāti,",
  "thag20.1:52.2_0": "… pe",
  "thag20.1:52.4_0": "kaṇha dukkhaṁ nigacchasi.",
  "thag20.1:53.1_0": "Yo ve buddhena codito,",
  "thag20.1:53.2_0": "bhikkhusaṅghassa pekkhato;",
  "thag20.1:53.3_0": "Migāramātupāsādaṁ,",
  "thag20.1:53.4_0": "pādaṅguṭṭhena kampayi.",
  "thag20.1:54.1_0": "Yo etamabhijānāti,",
  "thag20.1:54.2_0": "… pe",
  "thag20.1:54.4_0": "kaṇha dukkhaṁ nigacchasi.",
  "thag20.1:55.1_0": "Yo vejayantapāsādaṁ,",
  "thag20.1:55.2_0": "pādaṅguṭṭhena kampayi;",
  "thag20.1:55.3_0": "Iddhibalenupatthaddho,",
  "thag20.1:55.4_0": "saṁvejesi ca devatā.",
  "thag20.1:56.1_0": "Yo etamabhijānāti,",
  "thag20.1:56.2_0": "… pe",
  "thag20.1:56.4_0": "kaṇha dukkhaṁ nigacchasi.",
  "thag20.1:57.1_0": "Yo vejayantapāsāde,",
  "thag20.1:57.2_0": "sakkaṁ so paripucchati;",
  "thag20.1:57.3_0": "Api āvuso jānāsi,",
  "thag20.1:57.4_0": "taṇhakkhayavimuttiyo;",
  "thag20.1:57.5_0": "Tassa sakko viyākāsi,",
  "thag20.1:57.6_0": "pañhaṁ puṭṭho yathātathaṁ”.",
  "thag20.1:58.1_0": "“Yo etamabhijānāti,",
  "thag20.1:58.2_0": "… pe",
  "thag20.1:58.4_0": "kaṇha dukkhaṁ nigacchasi.",
  "thag20.1:59.1_0": "Yo brahmānaṁ paripucchati,",
  "thag20.1:59.2_0": "Sudhammāyaṁ ṭhito sabhaṁ;",
  "thag20.1:59.3_0": "Ajjāpi tyāvuso sā diṭṭhi,",
  "thag20.1:59.4_0": "Yā te diṭṭhi pure ahu;",
  "thag20.1:59.5_0": "Passasi vītivattantaṁ,",
  "thag20.1:59.6_0": "Brahmaloke pabhassaraṁ.",
  "thag20.1:60.1_0": "Tassa brahmā viyākāsi,",
  "thag20.1:60.2_0": "pañhaṁ puṭṭho yathātathaṁ;",
  "thag20.1:60.3_0": "Na me mārisa sā diṭṭhi,",
  "thag20.1:60.4_0": "yā me diṭṭhi pure ahu.",
  "thag20.1:61.1_0": "Passāmi vītivattantaṁ,",
  "thag20.1:61.2_0": "brahmaloke pabhassaraṁ;",
  "thag20.1:61.3_0": "Sohaṁ ajja kathaṁ vajjaṁ,",
  "thag20.1:61.4_0": "ahaṁ niccomhi sassato”.",
  "thag20.1:62.1_0": "“Yo etamabhijānāti,",
  "thag20.1:62.2_0": "… pe",
  "thag20.1:62.4_0": "kaṇha dukkhaṁ nigacchasi.",
  "thag20.1:63.1_0": "Yo mahāneruno kūṭaṁ,",
  "thag20.1:63.2_0": "vimokkhena aphassayi;",
  "thag20.1:63.3_0": "Vanaṁ pubbavidehānaṁ,",
  "thag20.1:63.4_0": "ye ca bhūmisayā narā.",
  "thag20.1:64.1_0": "Yo etamabhijānāti,",
  "thag20.1:64.2_0": "bhikkhu buddhassa sāvako;",
  "thag20.1:64.3_0": "Tādisaṁ bhikkhumāsajja,",
  "thag20.1:64.4_0": "kaṇha dukkhaṁ nigacchasi.",
  "thag20.1:65.1_0": "Na ve aggi cetayati,",
  "thag20.1:65.2_0": "ahaṁ bālaṁ ḍahāmīti;",
  "thag20.1:65.3_0": "Bālova jalitaṁ aggiṁ,",
  "thag20.1:65.4_0": "āsajja naṁ paḍayhati.",
  "thag20.1:66.1_0": "Evamevaṁ tuvaṁ māra,",
  "thag20.1:66.2_0": "āsajja naṁ tathāgataṁ;",
  "thag20.1:66.3_0": "Sayaṁ ḍahissasi attānaṁ,",
  "thag20.1:66.4_0": "bālo aggiṁva samphusaṁ.",
  "thag20.1:67.1_0": "Apuññaṁ pasavī māro,",
  "thag20.1:67.2_0": "āsajja naṁ tathāgataṁ;",
  "thag20.1:67.3_0": "Kiṁ nu maññasi pāpima,",
  "thag20.1:67.4_0": "na me pāpaṁ vipaccati.",
  "thag20.1:68.1_0": "Karato te cīyate pāpaṁ,",
  "thag20.1:68.2_0": "cirarattāya antaka;",
  "thag20.1:68.3_0": "Māra nibbinda buddhamhā,",
  "thag20.1:68.4_0": "āsaṁ mākāsi bhikkhusu”.",
  "thag20.1:69.1_0": "“Iti māraṁ atajjesi,",
  "thag20.1:69.2_0": "bhikkhu bhesakaḷāvane;",
  "thag20.1:69.3_0": "Tato so dummano yakkho,",
  "thag20.1:69.4_0": "tatthevantaradhāyathā”ti.",
  "thag20.1:69.5_0": "Itthaṁ sudaṁ āyasmā mahāmoggallāno thero gāthāyo abhāsitthāti.",
  "thag20.1:69.6_0": "Saṭṭhinipāto niṭṭhito.",
  "thag20.1:70.0_0": "Tatruddānaṁ",
  "thag20.1:70.1_0": "Saṭṭhikamhi nipātamhi,",
  "thag20.1:70.2_0": "moggallāno mahiddhiko;",
  "thag20.1:70.3_0": "Ekova theragāthāyo,",
  "thag20.1:70.4_0": "aṭṭhasaṭṭhi bhavanti tāti."
}