{
  "thag4.8:0.1_0": "Theragāthā 4.8",
  "thag4.8:0.2_0": "Catukkanipāta",
  "thag4.8:0.3_0": "Paṭhamavagga",
  "thag4.8:0.4_0": "Rāhulattheragāthā",
  "thag4.8:1.1_0": "“Ubhayeneva sampanno,",
  "thag4.8:1.2_0": "rāhulabhaddoti maṁ vidū;",
  "thag4.8:1.3_0": "Yañcamhi putto buddhassa,",
  "thag4.8:1.4_0": "yañca dhammesu cakkhumā.",
  "thag4.8:2.1_0": "Yañca me āsavā khīṇā,",
  "thag4.8:2.2_0": "yañca natthi punabbhavo;",
  "thag4.8:2.3_0": "Arahā dakkhiṇeyyomhi,",
  "thag4.8:2.4_0": "tevijjo amataddaso.",
  "thag4.8:3.1_0": "Kāmandhā jālapacchannā,",
  "thag4.8:3.2_0": "taṇhāchādanachāditā;",
  "thag4.8:3.3_0": "Pamattabandhunā baddhā,",
  "thag4.8:3.4_0": "macchāva kumināmukhe.",
  "thag4.8:4.1_0": "Taṁ kāmaṁ ahamujjhitvā,",
  "thag4.8:4.2_0": "chetvā mārassa bandhanaṁ;",
  "thag4.8:4.3_0": "Samūlaṁ taṇhamabbuyha,",
  "thag4.8:4.4_0": "sītibhūtosmi nibbuto”ti.",
  "thag4.8:4.5_0": "… Rāhulo thero …."
}