{
  "atk-vin02a18:0_0": "5. Pāṭidesanīyakaṇḍaṁ (bhikkhunīvibhaṅgavaṇṇanā)",
  "atk-vin02a18:1_0": "Pāṭidesanīyasikkhāpadavaṇṇanā",
  "atk-vin02a18:2_0": "Pāṭidesanīyā nāma, khuddakānaṁ anantarā;",
  "atk-vin02a18:3_0": "Ye dhammā aṭṭha āruḷhā, saṅkhepeneva saṅgahaṁ;",
  "atk-vin02a18:4_0": "Tesaṁ pavattate esā, saṅkhepeneva vaṇṇanā.",
  "atk-vin02a18:5_0": "1228.",
  "atk-vin02a18:5_1": "Yāni hi ettha pāḷiyaṁ sappitelādīni niddiṭṭhāni,",
  "atk-vin02a18:5_2": "tāniyeva viññāpetvā bhuñjantiyā pāṭidesanīyā.",
  "atk-vin02a18:5_3": "Pāḷivinimuttakesu pana sabbesu dukkaṭaṁ.",
  "atk-vin02a18:5_4": "Sesamettha uttānameva.",
  "atk-vin02a18:5_5": "Aṭṭhavidhampi panetaṁ pāṭidesanīyaṁ catusamuṭṭhānaṁ –",
  "atk-vin02a18:5_6": "kāyato kāyavācato kāyacittato kāyavācācittato ca samuṭṭhāti,",
  "atk-vin02a18:5_7": "kiriyaṁ nosaññāvimokkhaṁ,",
  "atk-vin02a18:5_8": "acittakaṁ,",
  "atk-vin02a18:5_9": "paṇṇattivajjaṁ,",
  "atk-vin02a18:5_10": "kāyakammaṁ,",
  "atk-vin02a18:5_11": "vacīkammaṁ, ticittaṁ,",
  "atk-vin02a18:5_12": "tivedananti.",
  "atk-vin02a18:6_0": "Pāṭidesanīyavaṇṇanā niṭṭhitā.",
  "atk-vin02a18:7_0": "Pāṭidesanīyakaṇḍaṁ niṭṭhitaṁ.",
  "atk-vin02a18:8_0": "Sekhiyā pana uddiṭṭhā, ye dhammā pañcasattati;",
  "atk-vin02a18:9_0": "Tesaṁ anantarāyeva, sattādhikaraṇavhayā.",
  "atk-vin02a18:10_0": "Mahāvibhaṅge yo vutto, tesaṁ atthavinicchayo;",
  "atk-vin02a18:11_0": "Bhikkhunīnaṁ vibhaṅgepi, tādisaṁyeva taṁ vidū.",
  "atk-vin02a18:12_0": "Yasmā tasmā visuṁ tesaṁ, dhammānaṁ atthavaṇṇanā;",
  "atk-vin02a18:13_0": "Na vuttā tattha yā vuttā, vuttāyeva hi sā idhāti.",
  "atk-vin02a18:14_0": "Samantapāsādikāya vinayasaṁvaṇṇanāya",
  "atk-vin02a18:15_0": "Bhikkhunīvibhaṅgavaṇṇanā niṭṭhitā.",
  "atk-vin02a18:16_0": "Sabbāsavapahaṁ esā, niṭṭhitā vaṇṇanā yathā;",
  "atk-vin02a18:17_0": "Sabbāsavapahaṁ maggaṁ, patvā passantu nibbutinti.",
  "atk-vin02a18:18_0": "Ubhatovibhaṅgaṭṭhakathā niṭṭhitā."
}