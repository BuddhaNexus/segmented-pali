{
  "tika-s0402t48:0_0": "(15) 5. Ābhāvaggo",
  "tika-s0402t48:1_0": "1-6. Ābhāsuttādivaṇṇanā",
  "tika-s0402t48:2_0": "141-146. Pañcamassa paṭhamādīni uttānatthāneva.",
  "tika-s0402t48:3_0": "Ābhāsuttādivaṇṇanā niṭṭhitā.",
  "tika-s0402t48:4_0": "7-10. Dutiyakālasuttādivaṇṇanā",
  "tika-s0402t48:5_0": "147- 150.",
  "tika-s0402t48:5_1": "Sattame paramatthato avijjamānasabhāvassa kālassa bhāvanādiyogo na sambhavatīti āha “kālāti tasmiṁ tasmiṁ",
  "tika-s0402t48:5_2": "kāle dhammassavanādivasena pavattānaṁ kusaladhammānaṁ etaṁ adhivacana”nti.",
  "tika-s0402t48:5_3": "Kālasahacaritā hi kusalā dhammā idha kāla-",
  "tika-s0402t48:5_4": "saddena gahitā aparassa asambhavato.",
  "tika-s0402t48:5_5": "Aṭṭhamādīni uttānatthāneva.",
  "tika-s0402t48:6_0": "Dutiyakālasuttādivaṇṇanā niṭṭhitā.",
  "tika-s0402t48:7_0": "Ābhāvaggavaṇṇanā niṭṭhitā.",
  "tika-s0402t48:8_0": "Tatiyapaṇṇāsakaṁ niṭṭhitaṁ.",
  "tika-s0402t48:9_0": "4. Catutthapaṇṇāsakaṁ"
}