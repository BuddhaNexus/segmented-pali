{
  "tha-ap144:0.1_0": "Therāpadāna",
  "tha-ap144:0.2_0": "Chattavagga",
  "tha-ap144:0.3_0": "2. Thambhāropakattheraapadāna",
  "tha-ap144:1.1_0": "“Nibbute lokanāthamhi,",
  "tha-ap144:1.2_0": "dhammadassīnarāsabhe;",
  "tha-ap144:1.3_0": "Āropesiṁ dhajatthambhaṁ,",
  "tha-ap144:1.4_0": "buddhaseṭṭhassa cetiye.",
  "tha-ap144:2.1_0": "Nisseṇiṁ māpayitvāna,",
  "tha-ap144:2.2_0": "thūpaseṭṭhaṁ samāruhiṁ;",
  "tha-ap144:2.3_0": "Jātipupphaṁ gahetvāna,",
  "tha-ap144:2.4_0": "thūpamhi abhiropayiṁ.",
  "tha-ap144:3.1_0": "Aho buddhā aho dhammā,",
  "tha-ap144:3.2_0": "aho no satthu sampadā;",
  "tha-ap144:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap144:3.4_0": "thūpapūjāyidaṁ phalaṁ.",
  "tha-ap144:4.1_0": "Catunnavutito kappe,",
  "tha-ap144:4.2_0": "thūpasīkhasanāmakā;",
  "tha-ap144:4.3_0": "Soḷasāsiṁsu rājāno,",
  "tha-ap144:4.4_0": "cakkavattī mahabbalā.",
  "tha-ap144:5.1_0": "Paṭisambhidā catasso,",
  "tha-ap144:5.2_0": "…pe…",
  "tha-ap144:5.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap144:6.1_0": "Itthaṁ sudaṁ āyasmā thambhāropako thero imā gāthāyo abhāsitthāti.",
  "tha-ap144:7.1_0": "Thambhāropakattherassāpadānaṁ dutiyaṁ."
}