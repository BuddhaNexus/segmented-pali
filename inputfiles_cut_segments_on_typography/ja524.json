{
  "ja524:0.1_0": "Jātaka",
  "ja524:0.2_0": "Cattālīsanipāta",
  "ja524:0.3_0": "Tesakuṇavagga",
  "ja524:0.4_0": "4. Saṅkhapālajātaka",
  "ja524:1.1_0": "“Ariyāvakāsosi pasannanetto,",
  "ja524:1.2_0": "Maññe bhavaṁ pabbajito kulamhā;",
  "ja524:1.3_0": "Kathaṁ nu vittāni pahāya bhoge,",
  "ja524:1.4_0": "Pabbaji nikkhamma gharā sapañña”.",
  "ja524:2.1_0": "“Sayaṁ vimānaṁ naradeva disvā,",
  "ja524:2.2_0": "Mahānubhāvassa mahoragassa;",
  "ja524:2.3_0": "Disvāna puññāna mahāvipākaṁ,",
  "ja524:2.4_0": "Saddhāyahaṁ pabbajitomhi rāja”.",
  "ja524:3.1_0": "“Na kāmakāmā na bhayā na dosā,",
  "ja524:3.2_0": "Vācaṁ musā pabbajitā bhaṇanti;",
  "ja524:3.3_0": "Akkhāhi me pucchito etamatthaṁ,",
  "ja524:3.4_0": "Sutvāna me jāyihitippasādo”.",
  "ja524:4.1_0": "“Vāṇijja raṭṭhādhipa gacchamāno,",
  "ja524:4.2_0": "Pathe addasāsimhi bhojaputte;",
  "ja524:4.3_0": "Pavaddhakāyaṁ uragaṁ mahantaṁ,",
  "ja524:4.4_0": "Ādāya gacchante pamodamāne.",
  "ja524:5.1_0": "Sohaṁ samāgamma janinda tehi,",
  "ja524:5.2_0": "Pahaṭṭhalomo avacamhi bhīto;",
  "ja524:5.3_0": "‘Kuhiṁ ayaṁ nīyati bhīmakāyo,",
  "ja524:5.4_0": "Nāgena kiṁ kāhatha bhojaputtā’.",
  "ja524:6.1_0": "‘Nāgo ayaṁ nīyati bhojanatthā,",
  "ja524:6.2_0": "Pavaddhakāyo urago mahanto;",
  "ja524:6.3_0": "Sāduñca thūlañca muduñca maṁsaṁ,",
  "ja524:6.4_0": "Na tvaṁ rasaññāsi videhaputta.",
  "ja524:7.1_0": "Ito mayaṁ gantvā sakaṁ niketaṁ,",
  "ja524:7.2_0": "Ādāya satthāni vikopayitvā;",
  "ja524:7.3_0": "Maṁsāni bhokkhāma pamodamānā,",
  "ja524:7.4_0": "Mayañhi ve sattavo pannagānaṁ’.",
  "ja524:8.1_0": "‘Sace ayaṁ nīyati bhojanatthā,",
  "ja524:8.2_0": "Pavaddhakāyo urago mahanto;",
  "ja524:8.3_0": "Dadāmi vo balibaddāni soḷasa,",
  "ja524:8.4_0": "Nāgaṁ imaṁ muñcatha bandhanasmā’.",
  "ja524:9.1_0": "‘Addhā hi no bhakkho ayaṁ manāpo,",
  "ja524:9.2_0": "Bahū ca no uragā bhuttapubbā;",
  "ja524:9.3_0": "Karoma te taṁ vacanaṁ aḷāra,",
  "ja524:9.4_0": "Mittañca no hohi videhaputta’.",
  "ja524:10.1_0": "Tadāssu te bandhanā mocayiṁsu,",
  "ja524:10.2_0": "Yaṁ natthuto paṭimokkassa pāse;",
  "ja524:10.3_0": "Mutto ca so bandhanā nāgarājā,",
  "ja524:10.4_0": "Pakkāmi pācīnamukho muhuttaṁ.",
  "ja524:11.1_0": "Gantvāna pācīnamukho muhuttaṁ,",
  "ja524:11.2_0": "Puṇṇehi nettehi palokayī maṁ;",
  "ja524:11.3_0": "Tadāssahaṁ piṭṭhito anvagacchiṁ,",
  "ja524:11.4_0": "Dasaṅguliṁ añjaliṁ paggahetvā.",
  "ja524:12.1_0": "‘Gaccheva kho tvaṁ taramānarūpo,",
  "ja524:12.2_0": "Mā taṁ amittā punaraggahesuṁ;",
  "ja524:12.3_0": "Dukkho hi luddehi punā samāgamo,",
  "ja524:12.4_0": "Adassanaṁ bhojaputtāna gaccha’.",
  "ja524:13.1_0": "Agamāsi so rahadaṁ vippasannaṁ,",
  "ja524:13.2_0": "Nīlobhāsaṁ ramaṇīyaṁ sutitthaṁ;",
  "ja524:13.3_0": "Samotataṁ jambuhi vetasāhi,",
  "ja524:13.4_0": "Pāvekkhi nittiṇṇabhayo patīto.",
  "ja524:14.1_0": "So taṁ pavissa na cirassa nāgo,",
  "ja524:14.2_0": "Dibbena me pāturahuṁ janinda;",
  "ja524:14.3_0": "Upaṭṭhahī maṁ pitaraṁva putto,",
  "ja524:14.4_0": "Hadayaṅgamaṁ kaṇṇasukhaṁ bhaṇanto.",
  "ja524:15.1_0": "‘Tvaṁ mesi mātā ca pitā aḷāra,",
  "ja524:15.2_0": "Abbhantaro pāṇadado sahāyo;",
  "ja524:15.3_0": "Sakañca iddhiṁ paṭilābhakosmi,",
  "ja524:15.4_0": "Aḷāra passa me nivesanāni;",
  "ja524:15.5_0": "Pahūtabhakkhaṁ bahuannapānaṁ,",
  "ja524:15.6_0": "Masakkasāraṁ viya vāsavassa.",
  "ja524:16.1_0": "Taṁ bhūmibhāgehi upetarūpaṁ,",
  "ja524:16.2_0": "Asakkharā ceva mudū subhā ca;",
  "ja524:16.3_0": "Nīcattiṇā apparajā ca bhūmi,",
  "ja524:16.4_0": "Pāsādikā yattha jahanti sokaṁ.",
  "ja524:17.1_0": "Anāvakulā veḷuriyūpanīlā,",
  "ja524:17.2_0": "Catuddisaṁ ambavanaṁ surammaṁ;",
  "ja524:17.3_0": "Pakkā ca pesī ca phalā suphullā,",
  "ja524:17.4_0": "Niccotukā dhārayantī phalāni’.",
  "ja524:18.1_0": "Tesaṁ vanānaṁ naradeva majjhe,",
  "ja524:18.2_0": "Nivesanaṁ bhassarasannikāsaṁ;",
  "ja524:18.3_0": "Rajataggaḷaṁ sovaṇṇamayaṁ uḷāraṁ,",
  "ja524:18.4_0": "Obhāsatī vijjurivantalikkhe.",
  "ja524:19.1_0": "Maṇīmayā soṇṇamayā uḷārā,",
  "ja524:19.2_0": "Anekacittā satataṁ sunimmitā;",
  "ja524:19.3_0": "Paripūrā kaññāhi alaṅkatābhi,",
  "ja524:19.4_0": "Suvaṇṇakāyūradharāhi rāja.",
  "ja524:20.1_0": "So saṅkhapālo taramānarūpo,",
  "ja524:20.2_0": "Pāsādamāruyha anomavaṇṇo;",
  "ja524:20.3_0": "Sahassathambhaṁ atulānubhāvaṁ,",
  "ja524:20.4_0": "Yatthassa bhariyā mahesī ahosi.",
  "ja524:21.1_0": "Ekā ca nārī taramānarūpā,",
  "ja524:21.2_0": "Ādāya veḷuriyamayaṁ mahagghaṁ;",
  "ja524:21.3_0": "Subhaṁ maṇiṁ jātimantūpapannaṁ,",
  "ja524:21.4_0": "Acoditā āsanamabbhihāsi.",
  "ja524:22.1_0": "Tato maṁ urago hatthe gahetvā,",
  "ja524:22.2_0": "Nisīdayī pāmukhaāsanasmiṁ;",
  "ja524:22.3_0": "‘Idamāsanaṁ atra bhavaṁ nisīdatu,",
  "ja524:22.4_0": "Bhavañhi me aññataro garūnaṁ’.",
  "ja524:23.1_0": "Aññā ca nārī taramānarūpā,",
  "ja524:23.2_0": "Ādāya vāriṁ upasaṅkamitvā;",
  "ja524:23.3_0": "Pādāni pakkhālayī me janinda,",
  "ja524:23.4_0": "Bhariyāva bhattū patino piyassa.",
  "ja524:24.1_0": "Aparā ca nārī taramānarūpā,",
  "ja524:24.2_0": "Paggayha sovaṇṇamayāya pātiyā;",
  "ja524:24.3_0": "Anekasūpaṁ vividhaṁ viyañjanaṁ,",
  "ja524:24.4_0": "Upanāmayī bhatta manuññarūpaṁ.",
  "ja524:25.1_0": "Turiyehi maṁ bhārata bhuttavantaṁ,",
  "ja524:25.2_0": "Upaṭṭhahuṁ bhattu mano viditvā;",
  "ja524:25.3_0": "Tatuttariṁ maṁ nipatī mahantaṁ,",
  "ja524:25.4_0": "Dibbehi kāmehi anappakehi.",
  "ja524:26.1_0": "‘Bhariyā mametā tisatā aḷāra,",
  "ja524:26.2_0": "Sabbattamajjhā padumuttarābhā;",
  "ja524:26.3_0": "Aḷāra etāssu te kāmakārā,",
  "ja524:26.4_0": "Dadāmi te tā paricārayassu’.",
  "ja524:27.1_0": "Saṁvaccharaṁ dibbarasānubhutvā,",
  "ja524:27.2_0": "Tadāssuhaṁ uttarimajjhabhāsiṁ;",
  "ja524:27.3_0": "‘Nāgassidaṁ kinti kathañca laddhaṁ,",
  "ja524:27.4_0": "Kathajjhagamāsi vimānaseṭṭhaṁ.",
  "ja524:28.1_0": "Adhiccaladdhaṁ pariṇāmajaṁ te,",
  "ja524:28.2_0": "Sayaṅkataṁ udāhu devehi dinnaṁ;",
  "ja524:28.3_0": "Pucchāmi taṁ nāgarājetamatthaṁ,",
  "ja524:28.4_0": "Kathajjhagamāsi vimānaseṭṭhaṁ’.",
  "ja524:29.1_0": "‘Nādhiccaladdhaṁ na pariṇāmajaṁ me,",
  "ja524:29.2_0": "Na sayaṅkataṁ nāpi devehi dinnaṁ;",
  "ja524:29.3_0": "Sakehi kammehi apāpakehi,",
  "ja524:29.4_0": "Puññehi me laddhamidaṁ vimānaṁ’.",
  "ja524:30.1_0": "‘Kiṁ te vataṁ kiṁ pana brahmacariyaṁ,",
  "ja524:30.2_0": "Kissa suciṇṇassa ayaṁ vipāko;",
  "ja524:30.3_0": "Akkhāhi me nāgarājetamatthaṁ,",
  "ja524:30.4_0": "Kathaṁ nu te laddhamidaṁ vimānaṁ’.",
  "ja524:31.1_0": "‘Rājā ahosiṁ magadhānamissaro,",
  "ja524:31.2_0": "Duyyodhano nāma mahānubhāvo;",
  "ja524:31.3_0": "So ittaraṁ jīvitaṁ saṁviditvā,",
  "ja524:31.4_0": "Asassataṁ vipariṇāmadhammaṁ.",
  "ja524:32.1_0": "Annañca pānañca pasannacitto,",
  "ja524:32.2_0": "Sakkacca dānaṁ vipulaṁ adāsiṁ;",
  "ja524:32.3_0": "Opānabhūtaṁ me gharaṁ tadāsi,",
  "ja524:32.4_0": "Santappitā samaṇabrāhmaṇā ca.",
  "ja524:33.1_0": "Mālañca gandhañca vilepanañca,",
  "ja524:33.2_0": "Padīpiyaṁ yānamupassayañca;",
  "ja524:33.3_0": "Acchādanaṁ seyyamathannapānaṁ,",
  "ja524:33.4_0": "Sakkacca dānāni adamha tattha.",
  "ja524:34.1_0": "Taṁ me vataṁ taṁ pana brahmacariyaṁ,",
  "ja524:34.2_0": "Tassa suciṇṇassa ayaṁ vipāko;",
  "ja524:34.3_0": "Teneva me laddhamidaṁ vimānaṁ,",
  "ja524:34.4_0": "Pahūtabhakkhaṁ bahuannapānaṁ;",
  "ja524:34.5_0": "Naccehi gītehi cupetarūpaṁ,",
  "ja524:34.6_0": "Ciraṭṭhitikaṁ na ca sassatāyaṁ’.",
  "ja524:35.1_0": "‘Appānubhāvā taṁ mahānubhāvaṁ,",
  "ja524:35.2_0": "Tejassinaṁ hanti atejavanto;",
  "ja524:35.3_0": "Kimeva dāṭhāvudha kiṁ paṭicca,",
  "ja524:35.4_0": "Hatthattamāgacchi vanibbakānaṁ.",
  "ja524:36.1_0": "Bhayaṁ nu te anvagataṁ mahantaṁ,",
  "ja524:36.2_0": "Tejo nu te nānvagaṁ dantamūlaṁ;",
  "ja524:36.3_0": "Kimeva dāṭhāvudha kiṁ paṭicca,",
  "ja524:36.4_0": "Kilesamāpajji vanibbakānaṁ’.",
  "ja524:37.1_0": "‘Na me bhayaṁ anvagataṁ mahantaṁ,",
  "ja524:37.2_0": "Tejo na sakkā mama tehi hantuṁ;",
  "ja524:37.3_0": "Satañca dhammāni sukittitāni,",
  "ja524:37.4_0": "Samuddavelāva duraccayāni.",
  "ja524:38.1_0": "Cātuddasiṁ pañcadasiṁ aḷāra,",
  "ja524:38.2_0": "Uposathaṁ niccamupāvasāmi;",
  "ja524:38.3_0": "Athāgamuṁ soḷasa bhojaputtā,",
  "ja524:38.4_0": "Rajjuṁ gahetvāna daḷhañca pāsaṁ.",
  "ja524:39.1_0": "Bhetvāna nāsaṁ atikassa rajjuṁ,",
  "ja524:39.2_0": "Nayiṁsu maṁ samparigayha luddā;",
  "ja524:39.3_0": "Etādisaṁ dukkhamahaṁ titikkhaṁ,",
  "ja524:39.4_0": "Uposathaṁ appaṭikopayanto’.",
  "ja524:40.1_0": "‘Ekāyane taṁ pathe addasaṁsu,",
  "ja524:40.2_0": "Balena vaṇṇena cupetarūpaṁ;",
  "ja524:40.3_0": "Siriyā paññāya ca bhāvitosi,",
  "ja524:40.4_0": "Kiṁ patthayaṁ nāga tapo karosi’.",
  "ja524:41.1_0": "‘Na puttahetū na dhanassa hetu,",
  "ja524:41.2_0": "Na āyuno cāpi aḷāra hetu;",
  "ja524:41.3_0": "Manussayoniṁ abhipatthayāno,",
  "ja524:41.4_0": "Tasmā parakkamma tapo karomi’.",
  "ja524:42.1_0": "‘Tvaṁ lohitakkho vihatantaraṁso,",
  "ja524:42.2_0": "Alaṅkato kappitakesamassu;",
  "ja524:42.3_0": "Surosito lohitacandanena,",
  "ja524:42.4_0": "Gandhabbarājāva disā pabhāsasi.",
  "ja524:43.1_0": "Deviddhipattosi mahānubhāvo,",
  "ja524:43.2_0": "Sabbehi kāmehi samaṅgibhūto;",
  "ja524:43.3_0": "Pucchāmi taṁ nāgarājetamatthaṁ,",
  "ja524:43.4_0": "Seyyo ito kena manussaloko’.",
  "ja524:44.1_0": "‘Aḷāra nāññatra manussalokā,",
  "ja524:44.2_0": "Suddhī va saṁvijjati saṁyamo vā;",
  "ja524:44.3_0": "Ahañca laddhāna manussayoniṁ,",
  "ja524:44.4_0": "Kāhāmi jātimaraṇassa antaṁ’.",
  "ja524:45.1_0": "‘Saṁvaccharo me vasato tavantike,",
  "ja524:45.2_0": "Annena pānena upaṭṭhitosmi;",
  "ja524:45.3_0": "Āmantayitvāna palemi nāga,",
  "ja524:45.4_0": "Cirappavutthosmi ahaṁ janinda’.",
  "ja524:46.1_0": "‘Puttā ca dārā anujīvino ca,",
  "ja524:46.2_0": "Niccānusiṭṭhā upatiṭṭhathetaṁ;",
  "ja524:46.3_0": "Kaccinnu taṁ nābhisapittha koci,",
  "ja524:46.4_0": "Piyañhi me dassanaṁ tuyhaṁ aḷāra’.",
  "ja524:47.1_0": "‘Yathāpi mātū ca pitū agāre,",
  "ja524:47.2_0": "Putto piyo paṭivihito vaseyya;",
  "ja524:47.3_0": "Tatopi mayhaṁ idhameva seyyo,",
  "ja524:47.4_0": "Cittañhi te nāga mayī pasannaṁ’.",
  "ja524:48.1_0": "‘Maṇī mamaṁ vijjati lohitaṅko,",
  "ja524:48.2_0": "Dhanāharo maṇiratanaṁ uḷāraṁ;",
  "ja524:48.3_0": "Ādāya tvaṁ gaccha sakaṁ niketaṁ,",
  "ja524:48.4_0": "Laddhā dhanaṁ taṁ maṇimossajassu’.",
  "ja524:49.1_0": "Diṭṭhā mayā mānusakāpi kāmā,",
  "ja524:49.2_0": "Asassatā vipariṇāmadhammā;",
  "ja524:49.3_0": "Ādīnavaṁ kāmaguṇesu disvā,",
  "ja524:49.4_0": "Saddhāyahaṁ pabbajitomhi rāja.",
  "ja524:50.1_0": "Dumapphalānīva patanti māṇavā,",
  "ja524:50.2_0": "Daharā ca vuddhā ca sarīrabhedā;",
  "ja524:50.3_0": "Etampi disvā pabbajitomhi rāja,",
  "ja524:50.4_0": "Apaṇṇakaṁ sāmaññameva seyyo”.",
  "ja524:51.1_0": "“Addhā have sevitabbā sapaññā,",
  "ja524:51.2_0": "Bahussutā ye bahuṭhānacintino;",
  "ja524:51.3_0": "Nāgañca sutvāna tavañcaḷāra,",
  "ja524:51.4_0": "Kāhāmi puññāni anappakāni”.",
  "ja524:52.1_0": "“Addhā have sevitabbā sapaññā,",
  "ja524:52.2_0": "Bahussutā ye bahuṭhānacintino;",
  "ja524:52.3_0": "Nāgañca sutvāna mamañca rāja,",
  "ja524:52.4_0": "Karohi puññāni anappakānī”ti.",
  "ja524:53.1_0": "Saṅkhapālajātakaṁ catutthaṁ."
}