{
  "tha-ap275:0.1_0": "Therāpadāna",
  "tha-ap275:0.2_0": "Suvaṇṇabibbohanavagga",
  "tha-ap275:0.3_0": "3 Caṅkoṭakiyattheraapadāna",
  "tha-ap275:1.1_0": "“Mahāsamuddaṁ nissāya,",
  "tha-ap275:1.2_0": "vasatī pabbatantare;",
  "tha-ap275:1.3_0": "Paccuggantvāna katvāna,",
  "tha-ap275:1.4_0": "caṅkoṭaka madāsahaṁ.",
  "tha-ap275:2.1_0": "Siddhatthassa mahesino,",
  "tha-ap275:2.2_0": "sabbasattānukampino;",
  "tha-ap275:2.3_0": "Pupphacaṅkoṭakaṁ datvā,",
  "tha-ap275:2.4_0": "kappaṁ saggamhi modahaṁ.",
  "tha-ap275:3.1_0": "Catunnavutito kappe,",
  "tha-ap275:3.2_0": "caṅkoṭakamadaṁ tadā;",
  "tha-ap275:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap275:3.4_0": "caṅkoṭakassidaṁ phalaṁ.",
  "tha-ap275:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap275:4.2_0": "…pe…",
  "tha-ap275:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap275:5.1_0": "Itthaṁ sudaṁ āyasmā caṅkoṭakiyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap275:6.1_0": "Caṅkoṭakiyattherassāpadānaṁ tatiyaṁ."
}