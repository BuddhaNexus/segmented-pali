{
  "tha-ap278:0.1_0": "Therāpadāna",
  "tha-ap278:0.2_0": "Suvaṇṇabibbohanavagga",
  "tha-ap278:0.3_0": "6 Potthakadāyakattheraapadāna",
  "tha-ap278:1.1_0": "“Satthāraṁ dhammamārabbha,",
  "tha-ap278:1.2_0": "saṅghañcāpi mahesinaṁ;",
  "tha-ap278:1.3_0": "Potthadānaṁ mayā dinnaṁ,",
  "tha-ap278:1.4_0": "dakkhiṇeyye anuttare.",
  "tha-ap278:2.1_0": "Ekanavutito kappe,",
  "tha-ap278:2.2_0": "yaṁ kammamakariṁ tadā;",
  "tha-ap278:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap278:2.4_0": "potthadānassidaṁ phalaṁ.",
  "tha-ap278:3.1_0": "Paṭisambhidā catasso,",
  "tha-ap278:3.2_0": "…pe…",
  "tha-ap278:3.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap278:4.1_0": "Itthaṁ sudaṁ āyasmā potthakadāyako thero imā gāthāyo abhāsitthāti.",
  "tha-ap278:5.1_0": "Potthakadāyakattherassāpadānaṁ chaṭṭhaṁ."
}