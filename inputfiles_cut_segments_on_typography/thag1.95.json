{
  "thag1.95:0.1_0": "Theragāthā 1.95",
  "thag1.95:0.2_0": "Ekakanipāta",
  "thag1.95:0.3_0": "Dasamavagga",
  "thag1.95:0.4_0": "Cakkhupālattheragāthā",
  "thag1.95:1.1_0": "“Andhohaṁ hatanettosmi,",
  "thag1.95:1.2_0": "kantāraddhānapakkhando;",
  "thag1.95:1.3_0": "Sayamānopi gacchissaṁ,",
  "thag1.95:1.4_0": "na sahāyena pāpenā”ti.",
  "thag1.95:1.5_0": "… Cakkhupālo thero …."
}