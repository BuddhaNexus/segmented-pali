{
  "atk-s0403a8:0_0": "(9) 4. Theravaggo",
  "atk-s0403a8:1_0": "1. Rajanīyasuttavaṇṇanā",
  "atk-s0403a8:2_0": "81. Catutthassa paṭhame rajanīyesūti rāgassa paccayesu ārammaṇesu. Sesesupi eseva nayo.",
  "atk-s0403a8:3_0": "2. Vītarāgasuttavaṇṇanā",
  "atk-s0403a8:4_0": "82. Dutiye makkhīti guṇamakkhako. Paḷāsīti yugaggāhalakkhaṇena paḷāsena samannāgato.",
  "atk-s0403a8:5_0": "3. Kuhakasuttavaṇṇanā",
  "atk-s0403a8:6_0": "83.",
  "atk-s0403a8:6_1": "Tatiye kuhakoti tīhi kuhanavatthūhi samannāgato.",
  "atk-s0403a8:6_2": "Lapakoti lābhasannissitāya lapanāya samannāgato.",
  "atk-s0403a8:6_3": "Nemittikoti nimittakiriyakārako.",
  "atk-s0403a8:6_4": "Nippesikoti nippesanakatāya samannāgato.",
  "atk-s0403a8:6_5": "Lābhena ca lābhaṁ nijigīsitāti lābhena lābhagavesako.",
  "atk-s0403a8:6_6": "Sukkapakkho vuttavipallāsavasena veditabbo.",
  "atk-s0403a8:6_7": "Catutthaṁ uttānameva.",
  "atk-s0403a8:7_0": "5. Akkhamasuttavaṇṇanā",
  "atk-s0403a8:8_0": "85.",
  "atk-s0403a8:8_1": "Pañcame akkhamo hoti rūpānanti rūpārammaṇānaṁ anadhivāsako hoti,",
  "atk-s0403a8:8_2": "tadārammaṇehi rāgādīhi abhibhuyyati.",
  "atk-s0403a8:8_3": "Eseva nayo sabbattha.",
  "atk-s0403a8:9_0": "6. Paṭisambhidāppattasuttavaṇṇanā",
  "atk-s0403a8:10_0": "86.",
  "atk-s0403a8:10_1": "Chaṭṭhe atthapaṭisambhidāppattoti pañcasu atthesu pabhedagataṁ ñāṇaṁ patto.",
  "atk-s0403a8:10_2": "Dhammapaṭisambhidāppattoti catubbidhe dhamme pabhedagataṁ ñāṇaṁ patto.",
  "atk-s0403a8:10_3": "Niruttipaṭisambhidāppattoti dhammaniruttīsu pabhedagataṁ ñāṇaṁ patto.",
  "atk-s0403a8:10_4": "Paṭibhānapaṭisambhidāppattoti tesu tīsu ñāṇesu pabhedagataṁ ñāṇaṁ patto.",
  "atk-s0403a8:10_5": "So pana tāni tīṇi ñāṇāneva jānāti,",
  "atk-s0403a8:10_6": "na tesaṁ kiccaṁ karoti.",
  "atk-s0403a8:10_7": "Uccāvacānīti mahantakhuddakāni.",
  "atk-s0403a8:10_8": "Kiṁkaraṇīyānīti iti kattabbāni.",
  "atk-s0403a8:11_0": "7. Sīlavantasuttavaṇṇanā",
  "atk-s0403a8:12_0": "87.",
  "atk-s0403a8:12_1": "Sattamaṁ uttānatthameva.",
  "atk-s0403a8:12_2": "Sīlaṁ panettha khīṇāsavasīlameva,",
  "atk-s0403a8:12_3": "bāhusaccampi khīṇāsavabāhusaccameva,",
  "atk-s0403a8:12_4": "vācāpi khīṇāsavassa kalyāṇavācāva,",
  "atk-s0403a8:12_5": "jhānānipi kiriyajjhānāneva kathitānīti veditabbāni.",
  "atk-s0403a8:13_0": "8. Therasuttavaṇṇanā",
  "atk-s0403a8:14_0": "88.",
  "atk-s0403a8:14_1": "Aṭṭhame theroti thirabhāvappatto.",
  "atk-s0403a8:14_2": "Rattaññūti pabbajitadivasato paṭṭhāya atikkantānaṁ bahūnaṁ rattīnaṁ ñātā.",
  "atk-s0403a8:14_3": "Ñātoti paññāto pākaṭo.",
  "atk-s0403a8:14_4": "Yasassīti yasanissito.",
  "atk-s0403a8:14_5": "Micchādiṭṭhikoti ayāthāvadiṭṭhiko.",
  "atk-s0403a8:14_6": "Saddhammā vuṭṭhāpetvāti dasakusalakammapathadhammato vuṭṭhāpetvā.",
  "atk-s0403a8:14_7": "Asaddhamme patiṭṭhāpetīti akusalakammapathesu patiṭṭhāpeti.",
  "atk-s0403a8:15_0": "9. Paṭhamasekhasuttavaṇṇanā",
  "atk-s0403a8:16_0": "89.",
  "atk-s0403a8:16_1": "Navame sekhassāti sikkhakassa sakaraṇīyassa.",
  "atk-s0403a8:16_2": "Parihānāyāti upariguṇehi parihānatthāya.",
  "atk-s0403a8:16_3": "Kammārāmatāti navakamme ramanakabhāvo.",
  "atk-s0403a8:16_4": "Bhassārāmatāti ālāpasallāpe ramanakabhāvo.",
  "atk-s0403a8:16_5": "Niddārāmatāti niddāyane ramanakabhāvo.",
  "atk-s0403a8:16_6": "Saṅgaṇikārāmatāti gaṇasaṅgaṇikāya ramanakabhāvo.",
  "atk-s0403a8:16_7": "Yathāvimuttaṁ cittaṁ na paccavekkhatīti yathā yaṁ cittaṁ vimuttaṁ,",
  "atk-s0403a8:16_8": "ye ca dosā pahīnā,",
  "atk-s0403a8:16_9": "guṇā ca paṭiladdhā,",
  "atk-s0403a8:16_10": "te paccavekkhitvā upariguṇapaṭilābhāya vāyāmaṁ na karotīti attho.",
  "atk-s0403a8:16_11": "Iti imasmiṁ sutte sattannaṁ sekhānaṁ upariguṇehi parihānikāraṇañca vuddhikāraṇañca kathitaṁ.",
  "atk-s0403a8:16_12": "Yañca nāma sekhassa parihānakāraṇaṁ,",
  "atk-s0403a8:16_13": "taṁ puthujjanassa paṭhamameva hotīti.",
  "atk-s0403a8:17_0": "10. Dutiyasekhasuttavaṇṇanā",
  "atk-s0403a8:18_0": "90.",
  "atk-s0403a8:18_1": "Dasame viyattoti byatto cheko.",
  "atk-s0403a8:18_2": "Kiṁkaraṇīyesūti iti kattabbesu.",
  "atk-s0403a8:18_3": "Cetosamathanti samādhikammaṭṭhānaṁ.",
  "atk-s0403a8:18_4": "Ananulomikenāti sāsanassa ananucchavikena.",
  "atk-s0403a8:18_5": "Atikālenāti atipātova.",
  "atk-s0403a8:18_6": "Atidivāti divā vuccati majjhanhiko,",
  "atk-s0403a8:18_7": "taṁ atikkamitvā.",
  "atk-s0403a8:18_8": "Ābhisallekhikāti ativiya kilesasallekhikā.",
  "atk-s0403a8:18_9": "Cetovivaraṇasappāyāti cittavivaraṇasaṅkhātānaṁ samathavipassanānaṁ sappāyā.",
  "atk-s0403a8:18_10": "Appicchakathāti appicchā hothāti kathanakathā.",
  "atk-s0403a8:18_11": "Santuṭṭhikathāti catūhi paccayehi santuṭṭhā hothāti kathanakathā.",
  "atk-s0403a8:18_12": "Pavivekakathāti tīhi vivekehi vivittā hothāti kathanakathā.",
  "atk-s0403a8:18_13": "Asaṁsaggakathāti pañcavidhena saṁsaggena asaṁsaṭṭhā hothāti kathanakathā.",
  "atk-s0403a8:18_14": "Vīriyārambhakathāti duvidhaṁ vīriyaṁ ārabhathāti kathanakathā.",
  "atk-s0403a8:18_15": "Sīlakathādīsu sīlaṁ ārabbha kathā sīlakathā.",
  "atk-s0403a8:18_16": "Samādhiṁ ārabbha,",
  "atk-s0403a8:18_17": "paññaṁ ārabbha,",
  "atk-s0403a8:18_18": "pañcavidhaṁ vimuttiṁ ārabbha,",
  "atk-s0403a8:18_19": "ekūnavīsatipaccavekkhaṇasaṅkhātaṁ vimuttiñāṇadassanaṁ ārabbha kathā vimuttiñāṇadassanakathā.",
  "atk-s0403a8:18_20": "Na nikāmalābhītiādīsu na icchiticchitalābhī,",
  "atk-s0403a8:18_21": "dukkhalābhī na vipulalābhīti attho.",
  "atk-s0403a8:18_22": "Sesaṁ uttānatthamevāti.",
  "atk-s0403a8:19_0": "Theravaggo catuttho."
}