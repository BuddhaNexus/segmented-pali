{
  "thig5.1:0.1_0": "Therīgāthā 5.1",
  "thig5.1:0.2_0": "Pañcakanipāta",
  "thig5.1:0.3_0": "Aññataratherīgāthā",
  "thig5.1:1.1_0": "“Paṇṇavīsativassāni,",
  "thig5.1:1.2_0": "yato pabbajitā ahaṁ;",
  "thig5.1:1.3_0": "Nāccharāsaṅghātamattampi,",
  "thig5.1:1.4_0": "cittassūpasamajjhagaṁ.",
  "thig5.1:2.1_0": "Aladdhā cetaso santiṁ,",
  "thig5.1:2.2_0": "kāmarāgenavassutā;",
  "thig5.1:2.3_0": "Bāhā paggayha kandantī,",
  "thig5.1:2.4_0": "vihāraṁ pāvisiṁ ahaṁ.",
  "thig5.1:3.1_0": "Sā bhikkhuniṁ upāgacchiṁ,",
  "thig5.1:3.2_0": "yā me saddhāyikā ahu;",
  "thig5.1:3.3_0": "Sā me dhammamadesesi,",
  "thig5.1:3.4_0": "khandhāyatanadhātuyo.",
  "thig5.1:4.1_0": "Tassā dhammaṁ suṇitvāna,",
  "thig5.1:4.2_0": "ekamante upāvisiṁ;",
  "thig5.1:4.3_0": "Pubbenivāsaṁ jānāmi,",
  "thig5.1:4.4_0": "dibbacakkhu visodhitaṁ.",
  "thig5.1:5.1_0": "Cetopariccañāṇañca,",
  "thig5.1:5.2_0": "sotadhātu visodhitā;",
  "thig5.1:5.3_0": "Iddhīpi me sacchikatā,",
  "thig5.1:5.4_0": "patto me āsavakkhayo;",
  "thig5.1:5.5_0": "Chaḷabhiññā sacchikatā,",
  "thig5.1:5.6_0": "kataṁ buddhassa sāsanan”ti.",
  "thig5.1:6.1_0": "… Aññatarā therī …."
}