{
  "anya-e0604n:0_0": "Namo tassa bhagavato arahato sammāsambuddhassa",
  "anya-e0604n:1_0": "Suttavandanā",
  "anya-e0604n:2_0": "1.",
  "anya-e0604n:3_0": "Dhammattha desanā vedha,",
  "anya-e0604n:4_0": "Catugambhīra duddasaṃ;",
  "anya-e0604n:5_0": "Dhammaṃ lokassa desentaṃ,",
  "anya-e0604n:6_0": "Dhammarājaṃ namāmahaṃ;",
  "anya-e0604n:7_0": "2.",
  "anya-e0604n:8_0": "Buddho paccekabuddho ca,",
  "anya-e0604n:9_0": "Arahā cakkavattīti;",
  "anya-e0604n:10_0": "Thūpāthūpārahaṃ vande,",
  "anya-e0604n:11_0": "Catuthūpārahesu taṃ;",
  "anya-e0604n:12_0": "3.",
  "anya-e0604n:13_0": "Petaseyyo kāmabhogī,",
  "anya-e0604n:14_0": "Sīhaseyyo tathāgato;",
  "anya-e0604n:15_0": "Sayantaṃ catuseyyesu,",
  "anya-e0604n:16_0": "Catutthena name jinaṃ;",
  "anya-e0604n:17_0": "4.",
  "anya-e0604n:18_0": "Aṇḍajā jalābujā ca;",
  "anya-e0604n:19_0": "Saṃsedajo papātikā;",
  "anya-e0604n:20_0": "Catuyoni pariccheda,",
  "anya-e0604n:21_0": "Ñāṇavantaṃ name jinaṃ;",
  "anya-e0604n:22_0": "5.",
  "anya-e0604n:23_0": "Sutabuddhonubuddho ca,",
  "anya-e0604n:24_0": "Pacceko jinabuddhoti;",
  "anya-e0604n:25_0": "Catutthaṃ catubuddhesu,",
  "anya-e0604n:26_0": "Vandāmi lokanāyakaṃ;",
  "anya-e0604n:27_0": "6.",
  "anya-e0604n:28_0": "Rāhuggaṃ attabhāgīnaṃ,",
  "anya-e0604n:29_0": "Mandhātā kāmabhogīnaṃ;",
  "anya-e0604n:30_0": "Māro adhipateyyānaṃ,",
  "anya-e0604n:31_0": "Buddho loke sadevake;",
  "anya-e0604n:32_0": "Catuagga paññattīnaṃ,",
  "anya-e0604n:33_0": "Aggapattaṃ name jinaṃ.",
  "anya-e0604n:34_0": "7.",
  "anya-e0604n:35_0": "Dānaṃ piyakathā attha,",
  "anya-e0604n:36_0": "Cariyā sadisattatā;",
  "anya-e0604n:37_0": "Catusaṅgaha vatthūhi,",
  "anya-e0604n:38_0": "Asaṅkhiyesu jātisu;",
  "anya-e0604n:39_0": "Lokassa saṅgahetāraṃ,",
  "anya-e0604n:40_0": "Vandāmi lokanāyakaṃ;",
  "anya-e0604n:41_0": "8.",
  "anya-e0604n:42_0": "Lobho doso ca mohoti,",
  "anya-e0604n:43_0": "Anto paccatthike tayo;",
  "anya-e0604n:44_0": "Dujjaye sabbaverīhi,",
  "anya-e0604n:45_0": "Sañjitaṃpi sudhiṃ name;",
  "anya-e0604n:46_0": "9.",
  "anya-e0604n:47_0": "Jāti puñña mahattāni,",
  "anya-e0604n:48_0": "Guṇamahattamuttamaṃ;",
  "anya-e0604n:49_0": "Timahatthehi sampannaṃ,",
  "anya-e0604n:50_0": "Vandāmi lokanāyakaṃ;",
  "anya-e0604n:51_0": "10.",
  "anya-e0604n:52_0": "Hetu phala sampadāyo,",
  "anya-e0604n:53_0": "Sattupakārasampadā;",
  "anya-e0604n:54_0": "Tisampadāhi sampannaṃ,",
  "anya-e0604n:55_0": "Vandāmi lokanāyakaṃ.",
  "anya-e0604n:56_0": "11.",
  "anya-e0604n:57_0": "Ñāṇappahānānubhāva,",
  "anya-e0604n:58_0": "Surūpakārasampadā;",
  "anya-e0604n:59_0": "Catuphala sampadāhi,",
  "anya-e0604n:60_0": "Taṃ sampannaṃ name jinaṃ.",
  "anya-e0604n:61_0": "12.",
  "anya-e0604n:62_0": "Āsayo ca payogoti,",
  "anya-e0604n:63_0": "Sattupakāra sampadā;",
  "anya-e0604n:64_0": "Duvidhā yassa sampannā,",
  "anya-e0604n:65_0": "Sammā maṃ pātu so jino.",
  "anya-e0604n:66_0": "13.",
  "anya-e0604n:67_0": "Mahosadhe vessantare,",
  "anya-e0604n:68_0": "Pacchimabhavike tathā;",
  "anya-e0604n:69_0": "Tijātīsu name vācaṃ,",
  "anya-e0604n:70_0": "Bhāsentaṃ jāyanakkhaṇe.",
  "anya-e0604n:71_0": "14.",
  "anya-e0604n:72_0": "Nemimhi sādhine ceva,",
  "anya-e0604n:73_0": "Mandhātari ca guttile;",
  "anya-e0604n:74_0": "Catūsu manujattena,",
  "anya-e0604n:75_0": "Devalokaṃ gataṃ name.",
  "anya-e0604n:76_0": "15.",
  "anya-e0604n:77_0": "Saṅkhāro rūpavikāro,",
  "anya-e0604n:78_0": "Lakkhaṇaṃ vānanissaṭaṃ;",
  "anya-e0604n:79_0": "Nibbānaṃ paññatticeti,",
  "anya-e0604n:80_0": "Pañca ñeyyantaguṃ name.",
  "anya-e0604n:81_0": "16.",
  "anya-e0604n:82_0": "Putta dāra pariccāgā,",
  "anya-e0604n:83_0": "Rajjaṅgaṃ jīva cāgīti;",
  "anya-e0604n:84_0": "Pañca mahāpariccāge,",
  "anya-e0604n:85_0": "Karaṃ sudukkare name.",
  "anya-e0604n:86_0": "17.",
  "anya-e0604n:87_0": "Rūpañca vedanā saññā,",
  "anya-e0604n:88_0": "Saṅkhārā viññāṇantime;",
  "anya-e0604n:89_0": "Pañcakkhandhe parijānaṃ,",
  "anya-e0604n:90_0": "Tipariññāhi taṃ name.",
  "anya-e0604n:91_0": "18.",
  "anya-e0604n:92_0": "Manussa deva gatiyo,",
  "anya-e0604n:93_0": "Peto ca nirayo tathā;",
  "anya-e0604n:94_0": "Tiracchānoti pañcetā,",
  "anya-e0604n:95_0": "Chindantaṃ gatiyo name;",
  "anya-e0604n:96_0": "19.",
  "anya-e0604n:97_0": "Devaputto kileso ca,",
  "anya-e0604n:98_0": "Abhisaṅkhāra mārako;",
  "anya-e0604n:99_0": "Khandho maccūti pañcete,",
  "anya-e0604n:100_0": "Māre vijitavaṃ name.",
  "anya-e0604n:101_0": "20.",
  "anya-e0604n:102_0": "Purebhattaṃ pacchābhattaṃ,",
  "anya-e0604n:103_0": "Purimañceva majjhimaṃ;",
  "anya-e0604n:104_0": "Pacchimanti sadā pañca,",
  "anya-e0604n:105_0": "Buddhakiccaṃ karaṃ name.",
  "anya-e0604n:106_0": "21.",
  "anya-e0604n:107_0": "Rūpā saddā gandhā rasā,",
  "anya-e0604n:108_0": "Phoṭṭhabbā ca manoramā;",
  "anya-e0604n:109_0": "Pañca kāmaguṇe hitvā,",
  "anya-e0604n:110_0": "Kheḷaṃva niggataṃ name.",
  "anya-e0604n:111_0": "22.",
  "anya-e0604n:112_0": "Rāgasallaṃ dosasallaṃ,",
  "anya-e0604n:113_0": "Mohā māno ca diṭṭhīti;",
  "anya-e0604n:114_0": "Vande vajjātivajjantaṃ,",
  "anya-e0604n:115_0": "Pañcasallapanūdanaṃ.",
  "anya-e0604n:116_0": "23.",
  "anya-e0604n:117_0": "Dīgho majjhima saṃyuttā,",
  "anya-e0604n:118_0": "Aṅguttaro ca khuddako;",
  "anya-e0604n:119_0": "Iti pañca nikāyehi,",
  "anya-e0604n:120_0": "Suvinentaṃ pajaṃ name.",
  "anya-e0604n:121_0": "24.",
  "anya-e0604n:122_0": "Issariyaṃ yaso dhammo,",
  "anya-e0604n:123_0": "Kāmo sirī payattīti;",
  "anya-e0604n:124_0": "Bhagyehi chahi sampannaṃ,",
  "anya-e0604n:125_0": "Vandāmi lokanāyakaṃ.",
  "anya-e0604n:126_0": "25.",
  "anya-e0604n:127_0": "Piyāpiyaṃ bhūtābhūtaṃ,",
  "anya-e0604n:128_0": "Atthānatthaṃ yathārahaṃ;",
  "anya-e0604n:129_0": "Missā chasu suvācaṃva,",
  "anya-e0604n:130_0": "Bhāsantaṃ atthakaṃ name.",
  "anya-e0604n:131_0": "26.",
  "anya-e0604n:132_0": "Alaso ca pamādo ca,",
  "anya-e0604n:133_0": "Anuṭṭhānaṃ asaṃyamo;",
  "anya-e0604n:134_0": "Niddā tantīti chiddehi,",
  "anya-e0604n:135_0": "Chahi muttaṃ sadā name.",
  "anya-e0604n:136_0": "27.",
  "anya-e0604n:137_0": "Mahākaruṇāsamāpatti,",
  "anya-e0604n:138_0": "Yamakappāṭihāriyaṃ;",
  "anya-e0604n:139_0": "Āsayānusaye ñāṇaṃ,",
  "anya-e0604n:140_0": "Indriyāna paropare;",
  "anya-e0604n:141_0": "Sabbaññutā nāvaraṇaṃ,",
  "anya-e0604n:142_0": "Chā sādhāraṇikaṃ name.",
  "anya-e0604n:143_0": "28.",
  "anya-e0604n:144_0": "Kāmacchando ca byāpādo,",
  "anya-e0604n:145_0": "Thinamiddhañca saṃsayo;",
  "anya-e0604n:146_0": "Avijjuddhacca kukkuccaṃ,",
  "anya-e0604n:147_0": "Chavinīvaraṇaṃ name.",
  "anya-e0604n:148_0": "29.",
  "anya-e0604n:149_0": "Iddhividhaṃ dibbasotaṃ,",
  "anya-e0604n:150_0": "Paracitta vijānanā;",
  "anya-e0604n:151_0": "Pubbenivāsānussati,",
  "anya-e0604n:152_0": "Dibbacakkhāsavakkhayo;",
  "anya-e0604n:153_0": "Chaḷabhiññāhi sampannaṃ,",
  "anya-e0604n:154_0": "Vandāmi purisuttamaṃ.",
  "anya-e0604n:155_0": "30.",
  "anya-e0604n:156_0": "Nīla pītā ca odātā,",
  "anya-e0604n:157_0": "Mañjiṭṭhā ca pabhassarā;",
  "anya-e0604n:158_0": "Lohitāti cha raṃsīhī,",
  "anya-e0604n:159_0": "Vijjotantaṃ sadā name.",
  "anya-e0604n:160_0": "31.",
  "anya-e0604n:161_0": "Assādādīnavo ceva,",
  "anya-e0604n:162_0": "Nissaraṇaphalaṃ tathā;",
  "anya-e0604n:163_0": "Upāyāṇattīti chadhā,",
  "anya-e0604n:164_0": "Saddhammadesakaṃ name.",
  "anya-e0604n:165_0": "32.",
  "anya-e0604n:166_0": "Rāgo doso ca moho ca,",
  "anya-e0604n:167_0": "Vitakkacariyā tathā;",
  "anya-e0604n:168_0": "Saddhā buddhīti bhājentaṃ,",
  "anya-e0604n:169_0": "Tā chaḷecariyā name.",
  "anya-e0604n:170_0": "33.",
  "anya-e0604n:171_0": "Rajataṃ kanakaṃ muttā,",
  "anya-e0604n:172_0": "Maṇi veḷuriyāni ca;",
  "anya-e0604n:173_0": "Vajirañca pavāḷanti,",
  "anya-e0604n:174_0": "Seṭṭhi sattadhano viya.",
  "anya-e0604n:175_0": "34.",
  "anya-e0604n:176_0": "Saddhāsīlaṃ sutaṃcāgo,",
  "anya-e0604n:177_0": "Hirī ottappiyaṃ dhanaṃ,",
  "anya-e0604n:178_0": "Paññādhananti sambuddhaṃ,",
  "anya-e0604n:179_0": "Vande sattamahādhanaṃ.",
  "anya-e0604n:180_0": "35.",
  "anya-e0604n:181_0": "Cakkaṃ hatthi asso maṇi,",
  "anya-e0604n:182_0": "Gahapatitthiyo tathā;",
  "anya-e0604n:183_0": "Supariṇāyakocāti,",
  "anya-e0604n:184_0": "Sattaratano cakkavattīva.",
  "anya-e0604n:185_0": "36.",
  "anya-e0604n:186_0": "Sati dhammavicayo ca,",
  "anya-e0604n:187_0": "Tatheva vīriyaṃ pīti;",
  "anya-e0604n:188_0": "Passaddhi samādhupekkhā,",
  "anya-e0604n:189_0": "Saddhamma cakkavattikaṃ;",
  "anya-e0604n:190_0": "Satta bojjhaṅgaratanaṃ,",
  "anya-e0604n:191_0": "Namāmi purisuttamaṃ.",
  "anya-e0604n:192_0": "37.",
  "anya-e0604n:193_0": "Yugandharo īsadharo,",
  "anya-e0604n:194_0": "Karavīko sudassano;",
  "anya-e0604n:195_0": "Nemindharo vinatako,",
  "anya-e0604n:196_0": "Assakaṇṇo mahānage;",
  "anya-e0604n:197_0": "Sattete hattharūpaṃva,",
  "anya-e0604n:198_0": "Sampassantaṃ jinaṃ name.",
  "anya-e0604n:199_0": "38.",
  "anya-e0604n:200_0": "Kaṇṇamuṇḍo anotatto,",
  "anya-e0604n:201_0": "Kuṇālo rathakārako;",
  "anya-e0604n:202_0": "Sīhappapāta chaddantā,",
  "anya-e0604n:203_0": "Mandākinī mahāsare;",
  "anya-e0604n:204_0": "Sattete sabbathā passaṃ,",
  "anya-e0604n:205_0": "Diṭṭheva pātikaṃ name.",
  "anya-e0604n:206_0": "39.",
  "anya-e0604n:207_0": "Manussattaṃ liṅgasampatti,",
  "anya-e0604n:208_0": "Hetu satthāradassanaṃ;",
  "anya-e0604n:209_0": "Pabbajjā guṇasampatti,",
  "anya-e0604n:210_0": "Adhikāro ca chandatā;",
  "anya-e0604n:211_0": "Aṭṭhadhamma samodhānā,",
  "anya-e0604n:212_0": "Bhinīhārantaguṃ name.",
  "anya-e0604n:213_0": "40.",
  "anya-e0604n:214_0": "Visaṭṭhaṃ mañju viññeyyaṃ,",
  "anya-e0604n:215_0": "Savanīyāvisāriṇā;",
  "anya-e0604n:216_0": "Gambhīro binduninnādo,",
  "anya-e0604n:217_0": "Aṭṭhaṅgikasaraṃ name.",
  "anya-e0604n:218_0": "41.",
  "anya-e0604n:219_0": "Cattāri rūpajhānāni,",
  "anya-e0604n:220_0": "Tathevārūpajhānāni;",
  "anya-e0604n:221_0": "Tā samāpattiyo aṭṭha,",
  "anya-e0604n:222_0": "Sevantaṃ ruciyā name.",
  "anya-e0604n:223_0": "42.",
  "anya-e0604n:224_0": "Aṇimā laṅghimā kammaṃ,",
  "anya-e0604n:225_0": "Mahimā pattimā tathā;",
  "anya-e0604n:226_0": "Īsitā vasitā ceva,",
  "anya-e0604n:227_0": "Yatthakāmāvasāyitā;",
  "anya-e0604n:228_0": "Aṭṭhissariya puṇṇattā,",
  "anya-e0604n:229_0": "Issarātissaraṃ name.",
  "anya-e0604n:230_0": "43.",
  "anya-e0604n:231_0": "Manomayiddhiñāṇañca,",
  "anya-e0604n:232_0": "Chaḷabhiññā vipassanā;",
  "anya-e0604n:233_0": "Aṭṭhavijjāhi sampannaṃ,",
  "anya-e0604n:234_0": "Vande tilokaketukaṃ.",
  "anya-e0604n:235_0": "44.",
  "anya-e0604n:236_0": "Jāti jarā rujā kālo,",
  "anya-e0604n:237_0": "Caturāpāyadukkhāti;",
  "anya-e0604n:238_0": "Aṭṭha saṃvegavatthūni,",
  "anya-e0604n:239_0": "Bhāsantaṃ vividhaṃ name.",
  "anya-e0604n:240_0": "45.",
  "anya-e0604n:241_0": "Suttaṅgaṃ veyyākaraṇaṃ,",
  "anya-e0604n:242_0": "Geyyaṃ gāthā ca jātakaṃ;",
  "anya-e0604n:243_0": "Abbhūtadhamma vedallā,",
  "anya-e0604n:244_0": "Udānamiti vuttakaṃ;",
  "anya-e0604n:245_0": "Navaṅgaṃ sabbalokassa,",
  "anya-e0604n:246_0": "Dadantaṃ sāsanaṃ name.",
  "anya-e0604n:247_0": "46.",
  "anya-e0604n:248_0": "Yā samāpattiyo aṭṭha,",
  "anya-e0604n:249_0": "Nirodho cāti te nava,",
  "anya-e0604n:250_0": "Anupubbavihāre taṃ,",
  "anya-e0604n:251_0": "Sevantaṃ ruciyā name.",
  "anya-e0604n:252_0": "47.",
  "anya-e0604n:253_0": "Ekattañceva nānattaṃ,",
  "anya-e0604n:254_0": "Kāyasaññīhi yogato,",
  "anya-e0604n:255_0": "Cattāro tathāruppā ca,",
  "anya-e0604n:256_0": "Asaññīcāti te nava,",
  "anya-e0604n:257_0": "Sattavāse vibhajitvā,",
  "anya-e0604n:258_0": "Pakāsentaṃ name jinaṃ.",
  "anya-e0604n:259_0": "48.",
  "anya-e0604n:260_0": "Seyyassa sadiso seyyo,",
  "anya-e0604n:261_0": "Hīnoti tividho vidho,",
  "anya-e0604n:262_0": "Tathā sadisahīnānaṃ,",
  "anya-e0604n:263_0": "Navavidhe nudaṃ name.",
  "anya-e0604n:264_0": "49.",
  "anya-e0604n:265_0": "Anatthaṃ me carati ca,",
  "anya-e0604n:266_0": "Tathā cari carissati,",
  "anya-e0604n:267_0": "Mittassa arinotvatthaṃ,",
  "anya-e0604n:268_0": "Navāghāte jahaṃ name.",
  "anya-e0604n:269_0": "50.",
  "anya-e0604n:270_0": "Dānaṃ sīlañca nikkhamaṃ,",
  "anya-e0604n:271_0": "Paññā vīriyapañcamaṃ,",
  "anya-e0604n:272_0": "Khantī saccamadhiṭṭhānaṃ,",
  "anya-e0604n:273_0": "Mettupekkhāti tādasa,",
  "anya-e0604n:274_0": "Pūretvā pāramī vande,",
  "anya-e0604n:275_0": "Sammāsambodhimajjhavaṃ;",
  "anya-e0604n:276_0": "51.",
  "anya-e0604n:277_0": "Kāḷāvakañca gaṅgeyyaṃ,",
  "anya-e0604n:278_0": "Paṇḍaraṃ tamba piṅgalaṃ,",
  "anya-e0604n:279_0": "Gandha maṅgala hemañca,",
  "anya-e0604n:280_0": "Uposatha chaddantime.",
  "anya-e0604n:281_0": "52.",
  "anya-e0604n:282_0": "Dhāreti dasaposānaṃ,",
  "anya-e0604n:283_0": "Nāgo kāḷāvako balaṃ;",
  "anya-e0604n:284_0": "Sahassakoṭiposānaṃ;",
  "anya-e0604n:285_0": "Chaddantovāranuttamo.",
  "anya-e0604n:286_0": "53.",
  "anya-e0604n:287_0": "Dasa sahassakoṭīnaṃ,",
  "anya-e0604n:288_0": "Posānaṃ baladhāraṇaṃ;",
  "anya-e0604n:289_0": "Chaddantānaṃ dasannañca,",
  "anya-e0604n:290_0": "Dasakāyabalaṃ name.",
  "anya-e0604n:291_0": "54.",
  "anya-e0604n:292_0": "Ṭhānāṭhāne vipāke ca,",
  "anya-e0604n:293_0": "Magge sabbatthagāminaṃ;",
  "anya-e0604n:294_0": "Nānādhātūsu lokesu,",
  "anya-e0604n:295_0": "Adhimuttimhi pāṇinaṃ;",
  "anya-e0604n:296_0": "55.",
  "anya-e0604n:297_0": "Paropariyatte ñāṇaṃ,",
  "anya-e0604n:298_0": "Indriyānañca pāṇinaṃ;",
  "anya-e0604n:299_0": "Jhānādīsu ñāṇaṃ pubbe,",
  "anya-e0604n:300_0": "Nivāse dibbacakkhu ca;",
  "anya-e0604n:301_0": "Āsavakkhayañāṇanti,",
  "anya-e0604n:302_0": "Dasabalañāṇaṃ name.",
  "anya-e0604n:303_0": "56.",
  "anya-e0604n:304_0": "Mūlagandho sāro pheggu,",
  "anya-e0604n:305_0": "Taco papaṭiko raso;",
  "anya-e0604n:306_0": "Pattaṃ pupphaṃ phalañceva,",
  "anya-e0604n:307_0": "Gandhagandhoti saṃvaraṃ;",
  "anya-e0604n:308_0": "Sugandhaṃ dasagandhehi,",
  "anya-e0604n:309_0": "Vilimpantaṃ sadā name.",
  "anya-e0604n:310_0": "57.",
  "anya-e0604n:311_0": "Samādhi ñāṇavipphārā,",
  "anya-e0604n:312_0": "Adhiṭṭhānaṃ vikubbanā;",
  "anya-e0604n:313_0": "Manomayāriyā iddhi,",
  "anya-e0604n:314_0": "Tathā kammavipākajā.",
  "anya-e0604n:315_0": "58.",
  "anya-e0604n:316_0": "Vijjāmayā puññavato;",
  "anya-e0604n:317_0": "Payoga paccayiddhiti;",
  "anya-e0604n:318_0": "Aggapatta dasiddhīnaṃ,",
  "anya-e0604n:319_0": "Pāṭiheraṃ karaṃ name.",
  "anya-e0604n:320_0": "59.",
  "anya-e0604n:321_0": "Muttā maṇi veḷuriyaṃ,",
  "anya-e0604n:322_0": "Saṅkho sallo pavāḷakaṃ;",
  "anya-e0604n:323_0": "Suvaṇṇaṃ rajataṃ lohi,",
  "anya-e0604n:324_0": "Taṅko masāragallanti;",
  "anya-e0604n:325_0": "Dasadhā ratanehiggaṃ,",
  "anya-e0604n:326_0": "Bhogiṃ paññāmaṇiṃ name.",
  "anya-e0604n:327_0": "60.",
  "anya-e0604n:328_0": "Sithilaṃ dhanitaṃ dīghaṃ,",
  "anya-e0604n:329_0": "Rassañca garukaṃ lahu;",
  "anya-e0604n:330_0": "Niggahitaṃ vimuttañca,",
  "anya-e0604n:331_0": "Sambandhañca vavatthitaṃ;",
  "anya-e0604n:332_0": "Vivarantaṃ name dhammaṃ,",
  "anya-e0604n:333_0": "Dasabyañjanabheditaṃ.",
  "anya-e0604n:334_0": "61.",
  "anya-e0604n:335_0": "Pasādojo madhuratā,",
  "anya-e0604n:336_0": "Samatā sukhumālatā;",
  "anya-e0604n:337_0": "Silesodāratā kanti,",
  "anya-e0604n:338_0": "Atthabyatti samādhayo;",
  "anya-e0604n:339_0": "Dasa saddaguṇopetaṃ,",
  "anya-e0604n:340_0": "Dhammaṃ pātu karaṃ name.",
  "anya-e0604n:341_0": "62.",
  "anya-e0604n:342_0": "Lobho doso moho māno,",
  "anya-e0604n:343_0": "Uddhaccaṃ diṭṭhi saṃsayo;",
  "anya-e0604n:344_0": "Ahirikamanottappaṃ,",
  "anya-e0604n:345_0": "Thinanti sabba dāhake;",
  "anya-e0604n:346_0": "Kilese dasa te saddhiṃ,",
  "anya-e0604n:347_0": "Vāsanāya jahaṃ name.",
  "anya-e0604n:348_0": "63.",
  "anya-e0604n:349_0": "Saterā daṇḍamaṇikā,",
  "anya-e0604n:350_0": "Macchavilolagaggarā;",
  "anya-e0604n:351_0": "Sukkhāsani kapisisā,",
  "anya-e0604n:352_0": "Vicakkā sañña kukkuṭā;",
  "anya-e0604n:353_0": "Navāsanīhi phālentaṃ,",
  "anya-e0604n:354_0": "Tibbaṃ paññāsaniṃ name.",
  "anya-e0604n:355_0": "64.",
  "anya-e0604n:356_0": "Rodanā kodhāvudho ca,",
  "anya-e0604n:357_0": "Ujjhantissariyaṃ tathā;",
  "anya-e0604n:358_0": "Paṭisaṅkhānaṃ,",
  "anya-e0604n:359_0": "Khanti aṭṭhabalesu taṃ;",
  "anya-e0604n:360_0": "Pathavīsadisaṃ vande,",
  "anya-e0604n:361_0": "Aṭṭhamena vināyakaṃ.",
  "anya-e0604n:362_0": "65.",
  "anya-e0604n:363_0": "Kāmarogo ca paṭigho,",
  "anya-e0604n:364_0": "Māno diṭṭhi ca saṃsayo;",
  "anya-e0604n:365_0": "Bhavarāgo avijjāti,",
  "anya-e0604n:366_0": "Sattānusayinaṃ name.",
  "anya-e0604n:367_0": "66.",
  "anya-e0604n:368_0": "Titthiyāpakatimahāsāvakā,",
  "anya-e0604n:369_0": "Aggasāvakā paccekabuddhasambuddhā;",
  "anya-e0604n:370_0": "Visiṭṭhaṃ sabbathā chasu,",
  "anya-e0604n:371_0": "Pubbe nivāsa viññūsu;",
  "anya-e0604n:372_0": "Tilokamakuṭaṃ name.",
  "anya-e0604n:373_0": "67.",
  "anya-e0604n:374_0": "Buddhacakkhu samantā ca,",
  "anya-e0604n:375_0": "Ñāṇaṃ yaṃ dibbacakkhu ca;",
  "anya-e0604n:376_0": "Dhammoti pañca cakkhūhi,",
  "anya-e0604n:377_0": "Dassāviṃ mārajiṃ name.",
  "anya-e0604n:378_0": "68.",
  "anya-e0604n:379_0": "Khuddakā khaṇikā okkantikā ca,",
  "anya-e0604n:380_0": "Pharaṇā tathā ubbegā;",
  "anya-e0604n:381_0": "Pañca pītīhi pinentaṃ,",
  "anya-e0604n:382_0": "Ruciyā ca namāmahaṃ.",
  "anya-e0604n:383_0": "69.",
  "anya-e0604n:384_0": "Dukkhaṃ samudayasaccaṃ,",
  "anya-e0604n:385_0": "Nirodho maggasaccakaṃ;",
  "anya-e0604n:386_0": "Catusaccamabhijānaṃ,",
  "anya-e0604n:387_0": "Catukiccehi taṃ name.",
  "anya-e0604n:388_0": "70.",
  "anya-e0604n:389_0": "Pīḷanā saṅkhatattho ca,",
  "anya-e0604n:390_0": "Tapo vipariṇāmanā;",
  "anya-e0604n:391_0": "Dukkhasaccaṃ catukkehi,",
  "anya-e0604n:392_0": "Vibhattāviṃ muniṃ name.",
  "anya-e0604n:393_0": "71.",
  "anya-e0604n:394_0": "Āyūhanaṃ nidānañca,",
  "anya-e0604n:395_0": "Saṃyogo palibodhanaṃ;",
  "anya-e0604n:396_0": "Catukkehi samudayaṃ,",
  "anya-e0604n:397_0": "Vibhattāviṃ muniṃ name.",
  "anya-e0604n:398_0": "72.",
  "anya-e0604n:399_0": "Nissaraṇañca viveko,",
  "anya-e0604n:400_0": "Asaṅkhatāmataṃ tathā;",
  "anya-e0604n:401_0": "Nirodhañca catukkehi,",
  "anya-e0604n:402_0": "Vibhattāviṃ muniṃ name.",
  "anya-e0604n:403_0": "73.",
  "anya-e0604n:404_0": "Niyyāniko ca hetvattho,",
  "anya-e0604n:405_0": "Dassanādhipateyyakaṃ;",
  "anya-e0604n:406_0": "Maggasaccaṃ catukkehi,",
  "anya-e0604n:407_0": "Vibhattāviṃ muniṃ name.",
  "anya-e0604n:408_0": "74.",
  "anya-e0604n:409_0": "Satthako ca asammoho,",
  "anya-e0604n:410_0": "Sappāyo ceva gocaro;",
  "anya-e0604n:411_0": "Catusampajaññā vinā,",
  "anya-e0604n:412_0": "Bhāviṃ name tathāgataṃ.",
  "anya-e0604n:413_0": "75.",
  "anya-e0604n:414_0": "Pahinākhila dukkhāhaṃ,",
  "anya-e0604n:415_0": "Bhavasāgara pāraguṃ;",
  "anya-e0604n:416_0": "Vande sāraguṇopetaṃ,",
  "anya-e0604n:417_0": "Tenamhi bhavapārago.",
  "anya-e0604n:418_0": "76.",
  "anya-e0604n:419_0": "Rūpārūpavilāsagga,",
  "anya-e0604n:420_0": "Rūpācinteyya saṃyuttaṃ;",
  "anya-e0604n:421_0": "Vande sāraguṇopetaṃ,",
  "anya-e0604n:422_0": "Tenamhātularūpavā.",
  "anya-e0604n:423_0": "77.",
  "anya-e0604n:424_0": "Iddhi iddhi vilāsagga,",
  "anya-e0604n:425_0": "Iddhi cinteyya saṃyuttaṃ;",
  "anya-e0604n:426_0": "Vande sāraguṇepetaṃ,",
  "anya-e0604n:427_0": "Tenamhā tulaiddhimā.",
  "anya-e0604n:428_0": "78.",
  "anya-e0604n:429_0": "Vācā vācā vilāsagga,",
  "anya-e0604n:430_0": "Vācā cinteyya saṃyuttaṃ;",
  "anya-e0604n:431_0": "Vande sāraguṇopetaṃ,",
  "anya-e0604n:432_0": "Tenamhā tulavācako.",
  "anya-e0604n:433_0": "79.",
  "anya-e0604n:434_0": "Ñāṇa ñāṇa vilāsagga,",
  "anya-e0604n:435_0": "Ñāṇācinteyya saṃyuttaṃ;",
  "anya-e0604n:436_0": "Vande sāraguṇopetaṃ,",
  "anya-e0604n:437_0": "Tenamhā tulañāṇavā.",
  "anya-e0604n:438_0": "80.",
  "anya-e0604n:439_0": "Sajjhaṃ hemañca ratanaṃ,",
  "anya-e0604n:440_0": "Gehaṃ vatthañca bhojanaṃ;",
  "anya-e0604n:441_0": "Tadaññepi yathācittaṃ,",
  "anya-e0604n:442_0": "Māpeyyāhaṃ name jinaṃ.",
  "anya-e0604n:443_0": "81.",
  "anya-e0604n:444_0": "Sajjhaṃ hemañca ratanaṃ,",
  "anya-e0604n:445_0": "Gehaṃ vatthañca bhojanaṃ;",
  "anya-e0604n:446_0": "Tadaññepi yathācittaṃ,",
  "anya-e0604n:447_0": "Māpeyyaṃ kammajiddhiyā.",
  "anya-e0604n:448_0": "82.",
  "anya-e0604n:449_0": "Sattabojjhaṅga ratano,",
  "anya-e0604n:450_0": "Saddhādiratano muni;",
  "anya-e0604n:451_0": "Satippabhutiratano,",
  "anya-e0604n:452_0": "Vande taṃ purisuttamaṃ.",
  "anya-e0604n:453_0": "83.",
  "anya-e0604n:454_0": "Cakkādisattaratanaṃ,",
  "anya-e0604n:455_0": "Muttādiratanaṃ subhaṃ;",
  "anya-e0604n:456_0": "Vatthappabhutiratanaṃ,",
  "anya-e0604n:457_0": "Chandakkhaṇe labhāmahaṃ.",
  "anya-e0604n:458_0": "84.",
  "anya-e0604n:459_0": "Sampuṇṇacittasaṅkappo,",
  "anya-e0604n:460_0": "Yathākaṅkhitamāpako,",
  "anya-e0604n:461_0": "Kenacinabhibhūto yo,",
  "anya-e0604n:462_0": "Sabbābhibhū name jinaṃ.",
  "anya-e0604n:463_0": "85.",
  "anya-e0604n:464_0": "Sampuṇṇacittasaṅkappo,",
  "anya-e0604n:465_0": "Yathākaṅkhitamāpako,",
  "anya-e0604n:466_0": "Kenacinabhibhūtamhi,",
  "anya-e0604n:467_0": "Sabbābhibhū bhave bhave.",
  "anya-e0604n:468_0": "86.",
  "anya-e0604n:469_0": "Dasa chaddantarājāva,",
  "anya-e0604n:470_0": "Dasa kāyabalo muni;",
  "anya-e0604n:471_0": "Dasa ñāṇabbalotulyo,",
  "anya-e0604n:472_0": "Vande taṃ samaṇuttamaṃ.",
  "anya-e0604n:473_0": "87.",
  "anya-e0604n:474_0": "Atikāyajavo buddho,",
  "anya-e0604n:475_0": "Raññājavanahaṃsato;",
  "anya-e0604n:476_0": "Kā kathā ñāṇavegassa,",
  "anya-e0604n:477_0": "Vande taṃ samaṇuttamaṃ.",
  "anya-e0604n:478_0": "88.",
  "anya-e0604n:479_0": "Chaddantanāgarājāva,",
  "anya-e0604n:480_0": "Bhave bhave mahabbalo;",
  "anya-e0604n:481_0": "Rājājavanahaṃsova,",
  "anya-e0604n:482_0": "Paramaggajavo bhave.",
  "anya-e0604n:483_0": "89.",
  "anya-e0604n:484_0": "Udakā kāsacārī ca,",
  "anya-e0604n:485_0": "Mahinimujjako jino;",
  "anya-e0604n:486_0": "Māpako ca yathā cittaṃ,",
  "anya-e0604n:487_0": "Vande taṃ ñeyyapāraguṃ.",
  "anya-e0604n:488_0": "90.",
  "anya-e0604n:489_0": "Udakā kāsacārī ca,",
  "anya-e0604n:490_0": "Mahinimujjako bhave;",
  "anya-e0604n:491_0": "Māpako ca yathācittaṃ,",
  "anya-e0604n:492_0": "Bhavaso kammajiddhiyā.",
  "anya-e0604n:493_0": "91.",
  "anya-e0604n:494_0": "Gāme vane ca sabbattha,",
  "anya-e0604n:495_0": "Devehi manussehi ca;",
  "anya-e0604n:496_0": "Ābhatānantalābhassa,",
  "anya-e0604n:497_0": "Sadā yassa mahesino;",
  "anya-e0604n:498_0": "Pattalābhaggataṃ lokaṃ,",
  "anya-e0604n:499_0": "Taṃ vande munipuṅgavaṃ.",
  "anya-e0604n:500_0": "92.",
  "anya-e0604n:501_0": "Gāme vane ca sabbattha,",
  "anya-e0604n:502_0": "Devehi manussehi ca;",
  "anya-e0604n:503_0": "Ābhatā nantalābho me,",
  "anya-e0604n:504_0": "Sadā hotu bhavābhave.",
  "anya-e0604n:505_0": "93.",
  "anya-e0604n:506_0": "Puññassimassa tejena,",
  "anya-e0604n:507_0": "Yathā cittaṃ samijjhatu;",
  "anya-e0604n:508_0": "Sabbicchā sabbacintā ca,",
  "anya-e0604n:509_0": "Khippaṃ me jātijātiyaṃ.",
  "anya-e0604n:510_0": "94.",
  "anya-e0604n:511_0": "Natthīti vacanaṃ dukkhaṃ,",
  "anya-e0604n:512_0": "Dehīti vacanaṃ tathā;",
  "anya-e0604n:513_0": "Tasmā natthīti dehīti,",
  "anya-e0604n:514_0": "Mā me hotu bhavābhave.",
  "anya-e0604n:515_0": "95.",
  "anya-e0604n:516_0": "Sabbaṃ paravasaṃ dukkhaṃ,",
  "anya-e0604n:517_0": "Sabbamissariyaṃ sukhaṃ;",
  "anya-e0604n:518_0": "Sabbaṃ paravasamatthu,",
  "anya-e0604n:519_0": "Sabbamissariyaṃ bhave.",
  "anya-e0604n:520_0": "96.",
  "anya-e0604n:521_0": "Vitakkento bhajjakāyo,",
  "anya-e0604n:522_0": "Sabbāvudha vāraṇo ca;",
  "anya-e0604n:523_0": "Chaddanta vāraṇabalo,",
  "anya-e0604n:524_0": "Bhaveyyaṃ jātijā tiyaṃ.",
  "anya-e0604n:525_0": "97.",
  "anya-e0604n:526_0": "Subhalakkhaṇasampanno,",
  "anya-e0604n:527_0": "Suvaṇṇavaṇṇavā bhave;",
  "anya-e0604n:528_0": "Brahmassaro karavika,",
  "anya-e0604n:529_0": "Bhāṇī ca jātijātiyaṃ.",
  "anya-e0604n:530_0": "98.",
  "anya-e0604n:531_0": "Bhūrimapañño sippānaṃ,",
  "anya-e0604n:532_0": "Sabbesaṃ kusalo bhave;",
  "anya-e0604n:533_0": "Visajjetuṃ samatthova,",
  "anya-e0604n:534_0": "Sabbapucchānaṃ ṭhānaso.",
  "anya-e0604n:535_0": "99.",
  "anya-e0604n:536_0": "Verādhaṃsīya bhogā ca,",
  "anya-e0604n:537_0": "Anantākhīṇa bhogavā;",
  "anya-e0604n:538_0": "Anantābhajja pariso,",
  "anya-e0604n:539_0": "Bhavaso pāpuṇe sivaṃ.",
  "anya-e0604n:540_0": "100.",
  "anya-e0604n:541_0": "Mā nasseyyaṃ pasayheyyaṃ,",
  "anya-e0604n:542_0": "Ussukkehi pyahaṃ saha;",
  "anya-e0604n:543_0": "Pañcaverehi koṭīhi,",
  "anya-e0604n:544_0": "Bhaṇḍaṃ vā akātukāmitaṃ.",
  "anya-e0604n:545_0": "101.",
  "anya-e0604n:546_0": "Meghaṃ vātañca ratanaṃ,",
  "anya-e0604n:547_0": "Dhaññaṃ vatthañca bhojanaṃ;",
  "anya-e0604n:548_0": "Sabbicchitaṃ tadaññampi,",
  "anya-e0604n:549_0": "Māpeyyaṃ kammajiddhiyā.",
  "anya-e0604n:550_0": "102.",
  "anya-e0604n:551_0": "Puññenetena nibbānaṃ,",
  "anya-e0604n:552_0": "Santaṃ pappomi tāvatā;",
  "anya-e0604n:553_0": "Bhaveyyaṃ sabbajātīsu,",
  "anya-e0604n:554_0": "Catusampattiyā sadā;",
  "anya-e0604n:555_0": "Catucakkena sampanno,",
  "anya-e0604n:556_0": "Saddhammehi ca sattahi.",
  "anya-e0604n:557_0": "103.",
  "anya-e0604n:558_0": "Sammādiṭṭhi vasupete,",
  "anya-e0604n:559_0": "Kulamhi seṭṭhasammate,",
  "anya-e0604n:560_0": "Sabbasakkata saṃsuddhe;",
  "anya-e0604n:561_0": "Bhave tihetusandhiko.",
  "anya-e0604n:562_0": "104.",
  "anya-e0604n:563_0": "Ghāsacchādanaṃ bhogañca,",
  "anya-e0604n:564_0": "Neva hatthena kātuna,",
  "anya-e0604n:565_0": "Bhuñjeyyamiddhiyātveva,",
  "anya-e0604n:566_0": "Māpetvā yāvadatthakaṃ.",
  "anya-e0604n:567_0": "105.",
  "anya-e0604n:568_0": "Manussānaṃ atikkamma,",
  "anya-e0604n:569_0": "Devānaṃ viya bhojanaṃ,",
  "anya-e0604n:570_0": "Vatthaṃ bhogo ca me hontu,",
  "anya-e0604n:571_0": "Parivārātisundarā.",
  "anya-e0604n:572_0": "106.",
  "anya-e0604n:573_0": "Dasāsuttaṃpi me bhogaṃ,",
  "anya-e0604n:574_0": "Pañcaverā bhave bhave;",
  "anya-e0604n:575_0": "Māgaṇheyyuñca nasseyyuṃ,",
  "anya-e0604n:576_0": "Asoko bhogahetume.",
  "anya-e0604n:577_0": "107.",
  "anya-e0604n:578_0": "Sabbaṅgasubha sampanno,",
  "anya-e0604n:579_0": "Siṅgīnikkha savaṇṇa vā;",
  "anya-e0604n:580_0": "Bāttiṃsa lakkhaṇūpeto,",
  "anya-e0604n:581_0": "Atappana rūpa vā bhave.",
  "anya-e0604n:582_0": "108.",
  "anya-e0604n:583_0": "Kāyo candanagandho ca,",
  "anya-e0604n:584_0": "Mukhaṃ uppalagandhikaṃ;",
  "anya-e0604n:585_0": "Aṭṭhaṅgiko karavika,",
  "anya-e0604n:586_0": "Mañjūghoso ca me hotu.",
  "anya-e0604n:587_0": "109.",
  "anya-e0604n:588_0": "Tikkha gambhīra pañño ca,",
  "anya-e0604n:589_0": "Hāsātijavapaññavā,",
  "anya-e0604n:590_0": "Bhūri nibbedha pañño ca;",
  "anya-e0604n:591_0": "Sabbapañhavisajjano.",
  "anya-e0604n:592_0": "111.",
  "anya-e0604n:593_0": "Anto soḷasavassassa,",
  "anya-e0604n:594_0": "Tipeṭakadharo bhave;",
  "anya-e0604n:595_0": "Sabbakammesu sippesu,",
  "anya-e0604n:596_0": "Vijjāṭhānesu pāragū."
}