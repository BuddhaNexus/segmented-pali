{
  "ja361:0.1_0": "Jātaka",
  "ja361:0.2_0": "Pañcakanipāta",
  "ja361:0.3_0": "Vaṇṇārohavagga",
  "ja361:0.4_0": "1. Vaṇṇārohajātaka",
  "ja361:1.1_0": "“Vaṇṇārohena jātiyā,",
  "ja361:1.2_0": "balanikkamanena ca;",
  "ja361:1.3_0": "Subāhu na mayā seyyo,",
  "ja361:1.4_0": "sudāṭha iti bhāsasi”.",
  "ja361:2.1_0": "“Vaṇṇārohena jātiyā,",
  "ja361:2.2_0": "balanikkamanena ca;",
  "ja361:2.3_0": "Sudāṭho na mayā seyyo,",
  "ja361:2.4_0": "subāhu iti bhāsasi.",
  "ja361:3.1_0": "Evañce maṁ viharantaṁ,",
  "ja361:3.2_0": "subāhu samma dubbhasi;",
  "ja361:3.3_0": "Na dānāhaṁ tayā saddhiṁ,",
  "ja361:3.4_0": "saṁvāsamabhirocaye.",
  "ja361:4.1_0": "Yo paresaṁ vacanāni,",
  "ja361:4.2_0": "saddaheyya yathātathaṁ;",
  "ja361:4.3_0": "Khippaṁ bhijjetha mittasmiṁ,",
  "ja361:4.4_0": "verañca pasave bahuṁ.",
  "ja361:5.1_0": "Na so mitto yo sadā appamatto,",
  "ja361:5.2_0": "Bhedāsaṅkī randhamevānupassī;",
  "ja361:5.3_0": "Yasmiñca setī urasīva putto,",
  "ja361:5.4_0": "Sa ve mitto yo abhejjo parehī”ti.",
  "ja361:6.1_0": "Vaṇṇārohajātakaṁ paṭhamaṁ."
}