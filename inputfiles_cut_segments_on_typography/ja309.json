{
  "ja309:0.1_0": "Jātaka",
  "ja309:0.2_0": "Catukkanipāta",
  "ja309:0.3_0": "Kāliṅgavagga",
  "ja309:0.4_0": "9. Chavakajātaka",
  "ja309:1.1_0": "“Sabbamidaṁ carimaṁ kataṁ,",
  "ja309:1.2_0": "Ubho dhammaṁ na passare;",
  "ja309:1.3_0": "Ubho pakatiyā cutā,",
  "ja309:1.4_0": "Yo cāyaṁ mantejjhāpeti;",
  "ja309:1.5_0": "Yo ca mantaṁ adhīyati”.",
  "ja309:2.1_0": "“Sālīnaṁ odanaṁ bhuñje,",
  "ja309:2.2_0": "suciṁ maṁsūpasecanaṁ;",
  "ja309:2.3_0": "Tasmā etaṁ na sevāmi,",
  "ja309:2.4_0": "dhammaṁ isīhi sevitaṁ”.",
  "ja309:3.1_0": "“Paribbaja mahā loko,",
  "ja309:3.2_0": "pacantaññepi pāṇino;",
  "ja309:3.3_0": "Mā taṁ adhammo ācarito,",
  "ja309:3.4_0": "asmā kumbhamivābhidā.",
  "ja309:4.1_0": "Dhiratthu taṁ yasalābhaṁ,",
  "ja309:4.2_0": "dhanalābhañca brāhmaṇa;",
  "ja309:4.3_0": "Yā vutti vinipātena,",
  "ja309:4.4_0": "adhammacaraṇena vā”ti.",
  "ja309:5.1_0": "Chavakajātakaṁ navamaṁ."
}