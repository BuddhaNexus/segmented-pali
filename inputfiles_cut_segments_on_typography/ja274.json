{
  "ja274:0.1_0": "Jātaka",
  "ja274:0.2_0": "Tikanipāta",
  "ja274:0.3_0": "Udapānavagga",
  "ja274:0.4_0": "4. Lolajātaka",
  "ja274:1.1_0": "“Kāyaṁ balākā sikhinī,",
  "ja274:1.2_0": "corī laṅghipitāmahā;",
  "ja274:1.3_0": "Oraṁ balāke āgaccha,",
  "ja274:1.4_0": "caṇḍo me vāyaso sakhā”.",
  "ja274:2.1_0": "“Nāhaṁ balākā sikhinī,",
  "ja274:2.2_0": "ahaṁ lolosmi vāyaso;",
  "ja274:2.3_0": "Akatvā vacanaṁ tuyhaṁ,",
  "ja274:2.4_0": "passa lūnosmi āgato”.",
  "ja274:3.1_0": "“Punapāpajjasī samma,",
  "ja274:3.2_0": "sīlañhi tava tādisaṁ;",
  "ja274:3.3_0": "Na hi mānusakā bhogā,",
  "ja274:3.4_0": "subhuñjā honti pakkhinā”ti.",
  "ja274:4.1_0": "Lolajātakaṁ catutthaṁ."
}