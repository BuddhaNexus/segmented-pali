{
  "ja385:0.1_0": "Jātaka",
  "ja385:0.2_0": "Chakkanipāta",
  "ja385:0.3_0": "Avāriyavagga",
  "ja385:0.4_0": "10. Nandiyamigarājajātaka",
  "ja385:1.1_0": "“Sace brāhmaṇa gacchesi,",
  "ja385:1.2_0": "sākete ajjunaṁ vanaṁ;",
  "ja385:1.3_0": "Vajjāsi nandiyaṁ nāma,",
  "ja385:1.4_0": "puttaṁ asmākamorasaṁ;",
  "ja385:1.5_0": "Mātā pitā ca te vuddhā,",
  "ja385:1.6_0": "te taṁ icchanti passituṁ”.",
  "ja385:2.1_0": "“Bhuttā mayā nivāpāni,",
  "ja385:2.2_0": "Rājino pānabhojanaṁ;",
  "ja385:2.3_0": "Taṁ rājapiṇḍaṁ avabhottuṁ,",
  "ja385:2.4_0": "Nāhaṁ brāhmaṇa mussahe”.",
  "ja385:3.1_0": "“Odahissāmahaṁ passaṁ,",
  "ja385:3.2_0": "khurappānissa rājino;",
  "ja385:3.3_0": "Tadāhaṁ sukhito mutto,",
  "ja385:3.4_0": "api passeyya mātaraṁ.",
  "ja385:4.1_0": "Migarājā pure āsiṁ,",
  "ja385:4.2_0": "kosalassa niketane;",
  "ja385:4.3_0": "Nandiyo nāma nāmena,",
  "ja385:4.4_0": "abhirūpo catuppado.",
  "ja385:5.1_0": "Taṁ maṁ vadhitumāgacchi,",
  "ja385:5.2_0": "dāyasmiṁ ajjune vane;",
  "ja385:5.3_0": "Dhanuṁ advejjhaṁ katvāna,",
  "ja385:5.4_0": "usuṁ sannayha kosalo.",
  "ja385:6.1_0": "Tassāhaṁ odahiṁ passaṁ,",
  "ja385:6.2_0": "khurappānissa rājino;",
  "ja385:6.3_0": "Tadāhaṁ sukhito mutto,",
  "ja385:6.4_0": "mātaraṁ daṭṭhumāgato”ti.",
  "ja385:7.1_0": "Nandiyamigarājajātakaṁ dasamaṁ.",
  "ja385:7.2_0": "Avāriyavaggo paṭhamo.",
  "ja385:8.1_0": "Tassuddānaṁ",
  "ja385:9.1_0": "Atha kujjharathesabha ketuvaro,",
  "ja385:9.2_0": "Sadarīmukha neru latā ca puna;",
  "ja385:9.3_0": "Apananda sirī ca sucittavaro,",
  "ja385:9.4_0": "Atha dhammika nandimigena dasāti."
}