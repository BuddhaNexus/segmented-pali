{
  "patthana13.9:0.1_0": "Paṭṭhānapakaraṇa",
  "patthana13.9:0.2_0": "Dhammānulomapaccanīya (3), Tikapaṭṭhānapāḷi (1)",
  "patthana13.9:0.3_0": "13.9. Dassanahetuttika",
  "patthana13.9:1.1_0": "Dassanena pahātabbahetukaṁ dhammaṁ paṭicca nadassanena pahātabbahetuko dhammo uppajjati hetupaccayā.",
  "patthana13.9:1.1_1": "(Saṅkhittaṁ.)",
  "patthana13.9:2.1_0": "Hetuyā chabbīsa,",
  "patthana13.9:2.1_1": "ārammaṇe ekavīsa,",
  "patthana13.9:2.1_2": "adhipatiyā ekūnavīsa …pe…",
  "patthana13.9:2.1_3": "vipāke tīṇi …pe…",
  "patthana13.9:2.1_4": "avigate chabbīsa.",
  "patthana13.9:2.1_5": "(Vitthāretabbaṁ.)"
}