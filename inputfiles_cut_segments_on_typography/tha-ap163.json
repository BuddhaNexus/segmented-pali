{
  "tha-ap163:0.1_0": "Therāpadāna",
  "tha-ap163:0.2_0": "Supāricariyavagga",
  "tha-ap163:0.3_0": "1. Supāricariyattheraapadāna",
  "tha-ap163:1.1_0": "“Padumo nāma nāmena,",
  "tha-ap163:1.2_0": "dvipadindo narāsabho;",
  "tha-ap163:1.3_0": "Pavanā abhinikkhamma,",
  "tha-ap163:1.4_0": "dhammaṁ deseti cakkhumā.",
  "tha-ap163:2.1_0": "Yakkhānaṁ samayo āsi,",
  "tha-ap163:2.2_0": "avidūre mahesino;",
  "tha-ap163:2.3_0": "Yena kiccena sampattā,",
  "tha-ap163:2.4_0": "ajjhāpekkhiṁsu tāvade.",
  "tha-ap163:3.1_0": "Buddhassa giramaññāya,",
  "tha-ap163:3.2_0": "amatassa ca desanaṁ;",
  "tha-ap163:3.3_0": "Pasannacitto sumano,",
  "tha-ap163:3.4_0": "apphoṭetvā upaṭṭhahiṁ.",
  "tha-ap163:4.1_0": "Suciṇṇassa phalaṁ passa,",
  "tha-ap163:4.2_0": "upaṭṭhānassa satthuno;",
  "tha-ap163:4.3_0": "Tiṁsakappasahassesu,",
  "tha-ap163:4.4_0": "duggatiṁ nupapajjahaṁ.",
  "tha-ap163:5.1_0": "Ūnatiṁse kappasate,",
  "tha-ap163:5.2_0": "samalaṅkatanāmako;",
  "tha-ap163:5.3_0": "Sattaratanasampanno,",
  "tha-ap163:5.4_0": "cakkavattī mahabbalo.",
  "tha-ap163:6.1_0": "Paṭisambhidā catasso,",
  "tha-ap163:6.2_0": "vimokkhāpi ca aṭṭhime;",
  "tha-ap163:6.3_0": "Chaḷabhiññā sacchikatā,",
  "tha-ap163:6.4_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap163:7.1_0": "Itthaṁ sudaṁ āyasmā supāricariyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap163:8.1_0": "Supāricariyattherassāpadānaṁ paṭhamaṁ."
}