{
  "tha-ap282:0.1_0": "Therāpadāna",
  "tha-ap282:0.2_0": "Suvaṇṇabibbohanavagga",
  "tha-ap282:0.3_0": "10 Taraṇīyattheraapadāna",
  "tha-ap282:1.1_0": "“Mahāpathamhi visame,",
  "tha-ap282:1.2_0": "setu kārāpito mayā;",
  "tha-ap282:1.3_0": "Taraṇatthāya lokassa,",
  "tha-ap282:1.4_0": "pasanno sehi pāṇibhi.",
  "tha-ap282:2.1_0": "Ekanavutito kappe,",
  "tha-ap282:2.2_0": "yo setu kārito mayā;",
  "tha-ap282:2.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap282:2.4_0": "setudānassidaṁ phalaṁ.",
  "tha-ap282:3.1_0": "Pañcapaññāsito kappe,",
  "tha-ap282:3.2_0": "eko āsiṁ samogadho;",
  "tha-ap282:3.3_0": "Sattaratanasampanno,",
  "tha-ap282:3.4_0": "cakkavattī mahabbalo.",
  "tha-ap282:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap282:4.2_0": "…pe…",
  "tha-ap282:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap282:5.1_0": "Itthaṁ sudaṁ āyasmā taraṇīyo thero imā gāthāyo abhāsitthāti.",
  "tha-ap282:6.1_0": "Taraṇīyattherassāpadānaṁ dasamaṁ.",
  "tha-ap282:6.2_0": "Suvaṇṇabibbohanavaggo aṭṭhavīsatimo.",
  "tha-ap282:7.1_0": "Tassuddānaṁ",
  "tha-ap282:8.1_0": "Suvaṇṇaṁ tilamuṭṭhi ca,",
  "tha-ap282:8.2_0": "caṅkoṭabbhañjanañjalī;",
  "tha-ap282:8.3_0": "Potthako citamāluvā,",
  "tha-ap282:8.4_0": "ekapuṇḍarī setunā;",
  "tha-ap282:8.5_0": "Dvecattālīsa gāthāyo,",
  "tha-ap282:8.6_0": "gaṇitāyo vibhāvibhīti.",
  "tha-ap282:9.1_0": "Ekādasamaṁ bhāṇavāraṁ."
}