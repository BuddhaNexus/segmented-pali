{
  "thig2.9:0.1_0": "Therīgāthā 2.9",
  "thig2.9:0.2_0": "Dukanipāta",
  "thig2.9:0.3_0": "Abhayātherīgāthā",
  "thig2.9:1.1_0": "“Abhaye bhiduro kāyo,",
  "thig2.9:1.2_0": "yattha satā puthujjanā;",
  "thig2.9:1.3_0": "Nikkhipissāmimaṁ dehaṁ,",
  "thig2.9:1.4_0": "sampajānā satīmatī.",
  "thig2.9:2.1_0": "Bahūhi dukkhadhammehi,",
  "thig2.9:2.2_0": "appamādaratāya me;",
  "thig2.9:2.3_0": "Taṇhakkhayo anuppatto,",
  "thig2.9:2.4_0": "kataṁ buddhassa sāsanan”ti.",
  "thig2.9:3.1_0": "… Abhayā therī …."
}