{
  "tha-ap325:0.1_0": "Therāpadāna",
  "tha-ap325:0.2_0": "Umāpupphiyavagga",
  "tha-ap325:0.3_0": "3. Hāsajanakattheraapadāna",
  "tha-ap325:1.1_0": "“Dumagge paṁsukūlakaṁ,",
  "tha-ap325:1.2_0": "laggaṁ disvāna satthuno;",
  "tha-ap325:1.3_0": "Añjaliṁ paggahetvāna,",
  "tha-ap325:1.4_0": "bhiyyo uccāritaṁ mayā.",
  "tha-ap325:2.1_0": "Dūrato pana disvāna,",
  "tha-ap325:2.2_0": "hāso me udapajjatha;",
  "tha-ap325:2.3_0": "Añjaliṁ paggahetvāna,",
  "tha-ap325:2.4_0": "bhiyyo cittaṁ pasādayiṁ.",
  "tha-ap325:3.1_0": "Ekanavutito kappe,",
  "tha-ap325:3.2_0": "yaṁ saññamalabhiṁ tadā;",
  "tha-ap325:3.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap325:3.4_0": "buddhasaññāyidaṁ phalaṁ.",
  "tha-ap325:4.1_0": "Paṭisambhidā catasso,",
  "tha-ap325:4.2_0": "…pe…",
  "tha-ap325:4.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap325:5.1_0": "Itthaṁ sudaṁ āyasmā hāsajanako thero imā gāthāyo abhāsitthāti.",
  "tha-ap325:6.1_0": "Hāsajanakattherassāpadānaṁ tatiyaṁ."
}