{
  "snp3.11:0.1_0": "Sutta Nipāta 3.11",
  "snp3.11:0.2_0": "Nālakasutta",
  "snp3.11:1.1_0": "Ānandajāte tidasagaṇe patīte,",
  "snp3.11:1.2_0": "Sakkañca indaṁ sucivasane ca deve;",
  "snp3.11:1.3_0": "Dussaṁ gahetvā atiriva thomayante,",
  "snp3.11:1.4_0": "Asito isi addasa divāvihāre.",
  "snp3.11:2.1_0": "Disvāna deve muditamane udagge,",
  "snp3.11:2.2_0": "Cittiṁ karitvāna idamavoca tattha;",
  "snp3.11:2.3_0": "“Kiṁ devasaṅgho atiriva kalyarūpo,",
  "snp3.11:2.4_0": "Dussaṁ gahetvā ramayatha kiṁ paṭicca.",
  "snp3.11:3.1_0": "Yadāpi āsī asurehi saṅgamo,",
  "snp3.11:3.2_0": "Jayo surānaṁ asurā parājitā;",
  "snp3.11:3.3_0": "Tadāpi netādiso lomahaṁsano,",
  "snp3.11:3.4_0": "Kimabbhutaṁ daṭṭhu marū pamoditā.",
  "snp3.11:4.1_0": "Seḷenti gāyanti ca vādayanti ca,",
  "snp3.11:4.2_0": "Bhujāni phoṭenti ca naccayanti ca;",
  "snp3.11:4.3_0": "Pucchāmi vohaṁ merumuddhavāsine,",
  "snp3.11:4.4_0": "Dhunātha me saṁsayaṁ khippa mārisā”.",
  "snp3.11:5.1_0": "“So bodhisatto ratanavaro atulyo,",
  "snp3.11:5.2_0": "Manussaloke hitasukhatthāya jāto;",
  "snp3.11:5.3_0": "Sakyāna gāme janapade lumbineyye,",
  "snp3.11:5.4_0": "Tenamha tuṭṭhā atiriva kalyarūpā.",
  "snp3.11:6.1_0": "So sabbasattuttamo aggapuggalo,",
  "snp3.11:6.2_0": "Narāsabho sabbapajānamuttamo;",
  "snp3.11:6.3_0": "Vattessati cakkamisivhaye vane,",
  "snp3.11:6.4_0": "Nadaṁva sīho balavā migābhibhū”.",
  "snp3.11:7.1_0": "Taṁ saddaṁ sutvā turitamavasarī so,",
  "snp3.11:7.2_0": "Suddhodanassa tada bhavanaṁ upāvisi;",
  "snp3.11:7.3_0": "Nisajja tattha idamavocāsi sakye,",
  "snp3.11:7.4_0": "“Kuhiṁ kumāro ahamapi daṭṭhukāmo”.",
  "snp3.11:8.1_0": "Tato kumāraṁ jalitamiva suvaṇṇaṁ,",
  "snp3.11:8.2_0": "Ukkāmukheva sukusalasampahaṭṭhaṁ;",
  "snp3.11:8.3_0": "Daddallamānaṁ siriyā anomavaṇṇaṁ,",
  "snp3.11:8.4_0": "Dassesu puttaṁ asitavhayassa sakyā.",
  "snp3.11:9.1_0": "Disvā kumāraṁ sikhimiva pajjalantaṁ,",
  "snp3.11:9.2_0": "Tārāsabhaṁva nabhasigamaṁ visuddhaṁ;",
  "snp3.11:9.3_0": "Suriyaṁ tapantaṁ saradarivabbhamuttaṁ,",
  "snp3.11:9.4_0": "Ānandajāto vipulamalattha pītiṁ.",
  "snp3.11:10.1_0": "Anekasākhañca sahassamaṇḍalaṁ,",
  "snp3.11:10.2_0": "Chattaṁ marū dhārayumantalikkhe;",
  "snp3.11:10.3_0": "Suvaṇṇadaṇḍā vītipatanti cāmarā,",
  "snp3.11:10.4_0": "Na dissare cāmarachattagāhakā.",
  "snp3.11:11.1_0": "Disvā jaṭī kaṇhasirivhayo isi,",
  "snp3.11:11.2_0": "Suvaṇṇanikkhaṁ viya paṇḍukambale;",
  "snp3.11:11.3_0": "Setañca chattaṁ dhariyanta muddhani,",
  "snp3.11:11.4_0": "Udaggacitto sumano paṭiggahe.",
  "snp3.11:12.1_0": "Paṭiggahetvā pana sakyapuṅgavaṁ,",
  "snp3.11:12.2_0": "Jigīsako lakkhaṇamantapāragū;",
  "snp3.11:12.3_0": "Pasannacitto giramabbhudīrayi,",
  "snp3.11:12.4_0": "“Anuttarāyaṁ dvipadānamuttamo”.",
  "snp3.11:13.1_0": "Athattano gamanamanussaranto,",
  "snp3.11:13.2_0": "Akalyarūpo gaḷayati assukāni;",
  "snp3.11:13.3_0": "Disvāna sakyā isimavocuṁ rudantaṁ,",
  "snp3.11:13.4_0": "“No ce kumāre bhavissati antarāyo”.",
  "snp3.11:14.1_0": "Disvāna sakye isimavoca akalye,",
  "snp3.11:14.2_0": "“Nāhaṁ kumāre ahitamanussarāmi;",
  "snp3.11:14.3_0": "Na cāpimassa bhavissati antarāyo,",
  "snp3.11:14.4_0": "Na orakāyaṁ adhimānasā bhavātha.",
  "snp3.11:15.1_0": "Sambodhiyaggaṁ phusissatāyaṁ kumāro,",
  "snp3.11:15.2_0": "So dhammacakkaṁ paramavisuddhadassī;",
  "snp3.11:15.3_0": "Vattessatāyaṁ bahujanahitānukampī,",
  "snp3.11:15.4_0": "Vitthārikassa bhavissati brahmacariyaṁ.",
  "snp3.11:16.1_0": "Mamañca āyu na ciramidhāvaseso,",
  "snp3.11:16.2_0": "Athantarā me bhavissati kālakiriyā;",
  "snp3.11:16.3_0": "Sohaṁ na sossaṁ asamadhurassa dhammaṁ,",
  "snp3.11:16.4_0": "Tenamhi aṭṭo byasanaṅgato aghāvī”.",
  "snp3.11:17.1_0": "So sākiyānaṁ vipulaṁ janetvā pītiṁ,",
  "snp3.11:17.2_0": "Antepuramhā niggamā brahmacārī;",
  "snp3.11:17.3_0": "So bhāgineyyaṁ sayaṁ anukampamāno,",
  "snp3.11:17.4_0": "Samādapesi asamadhurassa dhamme.",
  "snp3.11:18.1_0": "“Buddhoti ghosaṁ yada parato suṇāsi,",
  "snp3.11:18.2_0": "Sambodhipatto vivarati dhammamaggaṁ;",
  "snp3.11:18.3_0": "Gantvāna tattha samayaṁ paripucchamāno,",
  "snp3.11:18.4_0": "Carassu tasmiṁ bhagavati brahmacariyaṁ”.",
  "snp3.11:19.1_0": "Tenānusiṭṭho hitamanena tādinā,",
  "snp3.11:19.2_0": "Anāgate paramavisuddhadassinā;",
  "snp3.11:19.3_0": "So nālako upacitapuññasañcayo,",
  "snp3.11:19.4_0": "Jinaṁ patikkhaṁ parivasi rakkhitindriyo.",
  "snp3.11:20.1_0": "Sutvāna ghosaṁ jinavaracakkavattane,",
  "snp3.11:20.2_0": "Gantvāna disvā isinisabhaṁ pasanno;",
  "snp3.11:20.3_0": "Moneyyaseṭṭhaṁ munipavaraṁ apucchi,",
  "snp3.11:20.4_0": "Samāgate asitāvhayassa sāsaneti.",
  "snp3.11:20.5_0": "Vatthugāthā niṭṭhitā.",
  "snp3.11:21.1_0": "“Aññātametaṁ vacanaṁ,",
  "snp3.11:21.2_0": "Asitassa yathātathaṁ;",
  "snp3.11:21.3_0": "Taṁ taṁ gotama pucchāmi,",
  "snp3.11:21.4_0": "Sabbadhammāna pāraguṁ.",
  "snp3.11:22.1_0": "Anagāriyupetassa,",
  "snp3.11:22.2_0": "Bhikkhācariyaṁ jigīsato;",
  "snp3.11:22.3_0": "Muni pabrūhi me puṭṭho,",
  "snp3.11:22.4_0": "Moneyyaṁ uttamaṁ padaṁ”.",
  "snp3.11:23.1_0": "“Moneyyaṁ te upaññissaṁ,",
  "snp3.11:23.2_0": "(iti bhagavā)",
  "snp3.11:23.3_0": "Dukkaraṁ durabhisambhavaṁ;",
  "snp3.11:23.4_0": "Handa te naṁ pavakkhāmi,",
  "snp3.11:23.5_0": "Santhambhassu daḷho bhava.",
  "snp3.11:24.1_0": "Samānabhāgaṁ kubbetha,",
  "snp3.11:24.2_0": "Gāme akkuṭṭhavanditaṁ;",
  "snp3.11:24.3_0": "Manopadosaṁ rakkheyya,",
  "snp3.11:24.4_0": "Santo anuṇṇato care.",
  "snp3.11:25.1_0": "Uccāvacā niccharanti,",
  "snp3.11:25.2_0": "Dāye aggisikhūpamā;",
  "snp3.11:25.3_0": "Nāriyo muniṁ palobhenti,",
  "snp3.11:25.4_0": "Tā su taṁ mā palobhayuṁ.",
  "snp3.11:26.1_0": "Virato methunā dhammā,",
  "snp3.11:26.2_0": "Hitvā kāme paropare;",
  "snp3.11:26.3_0": "Aviruddho asāratto,",
  "snp3.11:26.4_0": "Pāṇesu tasathāvare.",
  "snp3.11:27.1_0": "Yathā ahaṁ tathā ete,",
  "snp3.11:27.2_0": "Yathā ete tathā ahaṁ;",
  "snp3.11:27.3_0": "Attānaṁ upamaṁ katvā,",
  "snp3.11:27.4_0": "Na haneyya na ghātaye.",
  "snp3.11:28.1_0": "Hitvā icchañca lobhañca,",
  "snp3.11:28.2_0": "Yattha satto puthujjano;",
  "snp3.11:28.3_0": "Cakkhumā paṭipajjeyya,",
  "snp3.11:28.4_0": "Tareyya narakaṁ imaṁ.",
  "snp3.11:29.1_0": "Ūnūdaro mitāhāro,",
  "snp3.11:29.2_0": "Appicchassa alolupo;",
  "snp3.11:29.3_0": "Sadā icchāya nicchāto,",
  "snp3.11:29.4_0": "Aniccho hoti nibbuto.",
  "snp3.11:30.1_0": "Sa piṇḍacāraṁ caritvā,",
  "snp3.11:30.2_0": "Vanantamabhihāraye;",
  "snp3.11:30.3_0": "Upaṭṭhito rukkhamūlasmiṁ,",
  "snp3.11:30.4_0": "Āsanūpagato muni.",
  "snp3.11:31.1_0": "Sa jhānapasuto dhīro,",
  "snp3.11:31.2_0": "Vanante ramito siyā;",
  "snp3.11:31.3_0": "Jhāyetha rukkhamūlasmiṁ,",
  "snp3.11:31.4_0": "Attānamabhitosayaṁ.",
  "snp3.11:32.1_0": "Tato ratyā vivasāne,",
  "snp3.11:32.2_0": "Gāmantamabhihāraye;",
  "snp3.11:32.3_0": "Avhānaṁ nābhinandeyya,",
  "snp3.11:32.4_0": "Abhihārañca gāmato.",
  "snp3.11:33.1_0": "Na munī gāmamāgamma,",
  "snp3.11:33.2_0": "Kulesu sahasā care;",
  "snp3.11:33.3_0": "Ghāsesanaṁ chinnakatho,",
  "snp3.11:33.4_0": "Na vācaṁ payutaṁ bhaṇe.",
  "snp3.11:34.1_0": "Alatthaṁ yadidaṁ sādhu,",
  "snp3.11:34.2_0": "Nālatthaṁ kusalaṁ iti;",
  "snp3.11:34.3_0": "Ubhayeneva so tādī,",
  "snp3.11:34.4_0": "Rukkhaṁvupanivattati.",
  "snp3.11:35.1_0": "Sa pattapāṇi vicaranto,",
  "snp3.11:35.2_0": "Amūgo mūgasammato;",
  "snp3.11:35.3_0": "Appaṁ dānaṁ na hīḷeyya,",
  "snp3.11:35.4_0": "Dātāraṁ nāvajāniyā.",
  "snp3.11:36.1_0": "Uccāvacā hi paṭipadā,",
  "snp3.11:36.2_0": "Samaṇena pakāsitā;",
  "snp3.11:36.3_0": "Na pāraṁ diguṇaṁ yanti,",
  "snp3.11:36.4_0": "Nayidaṁ ekaguṇaṁ mutaṁ.",
  "snp3.11:37.1_0": "Yassa ca visatā natthi,",
  "snp3.11:37.2_0": "Chinnasotassa bhikkhuno;",
  "snp3.11:37.3_0": "Kiccākiccappahīnassa,",
  "snp3.11:37.4_0": "Pariḷāho na vijjati.",
  "snp3.11:38.1_0": "Moneyyaṁ te upaññissaṁ,",
  "snp3.11:38.2_0": "Khuradhārūpamo bhave;",
  "snp3.11:38.3_0": "Jivhāya tālumāhacca,",
  "snp3.11:38.4_0": "Udare saññato siyā.",
  "snp3.11:39.1_0": "Alīnacitto ca siyā,",
  "snp3.11:39.2_0": "Na cāpi bahu cintaye;",
  "snp3.11:39.3_0": "Nirāmagandho asito,",
  "snp3.11:39.4_0": "Brahmacariyaparāyaṇo.",
  "snp3.11:40.1_0": "Ekāsanassa sikkhetha,",
  "snp3.11:40.2_0": "Samaṇūpāsanassa ca;",
  "snp3.11:40.3_0": "Ekattaṁ monamakkhātaṁ,",
  "snp3.11:40.4_0": "Eko ce abhiramissasi;",
  "snp3.11:40.5_0": "Atha bhāhisi dasadisā.",
  "snp3.11:41.1_0": "Sutvā dhīrānaṁ nighosaṁ,",
  "snp3.11:41.2_0": "Jhāyīnaṁ kāmacāginaṁ;",
  "snp3.11:41.3_0": "Tato hiriñca saddhañca,",
  "snp3.11:41.4_0": "Bhiyyo kubbetha māmako.",
  "snp3.11:42.1_0": "Taṁ nadīhi vijānātha,",
  "snp3.11:42.2_0": "Sobbhesu padaresu ca;",
  "snp3.11:42.3_0": "Saṇantā yanti kusobbhā,",
  "snp3.11:42.4_0": "Tuṇhī yanti mahodadhī.",
  "snp3.11:43.1_0": "Yadūnakaṁ taṁ saṇati,",
  "snp3.11:43.2_0": "Yaṁ pūraṁ santameva taṁ;",
  "snp3.11:43.3_0": "Aḍḍhakumbhūpamo bālo,",
  "snp3.11:43.4_0": "Rahado pūrova paṇḍito.",
  "snp3.11:44.1_0": "Yaṁ samaṇo bahuṁ bhāsati,",
  "snp3.11:44.2_0": "Upetaṁ atthasañhitaṁ;",
  "snp3.11:44.3_0": "Jānaṁ so dhammaṁ deseti,",
  "snp3.11:44.4_0": "Jānaṁ so bahu bhāsati.",
  "snp3.11:45.1_0": "Yo ca jānaṁ saṁyatatto,",
  "snp3.11:45.2_0": "Jānaṁ na bahu bhāsati;",
  "snp3.11:45.3_0": "Sa munī monamarahati,",
  "snp3.11:45.4_0": "Sa munī monamajjhagā”ti.",
  "snp3.11:45.5_0": "Nālakasuttaṁ ekādasamaṁ."
}