{
  "vv34:0.1_0": "Vimānavatthu",
  "vv34:0.2_0": "Itthivimāna",
  "vv34:0.3_0": "Pāricchattakavagga",
  "vv34:0.4_0": "6. Daddallavimānavatthu",
  "vv34:1.1_0": "“Daddallamānā vaṇṇena,",
  "vv34:1.2_0": "yasasā ca yasassinī;",
  "vv34:1.3_0": "Sabbe deve tāvatiṁse,",
  "vv34:1.4_0": "vaṇṇena atirocasi.",
  "vv34:2.1_0": "Dassanaṁ nābhijānāmi,",
  "vv34:2.2_0": "idaṁ paṭhamadassanaṁ;",
  "vv34:2.3_0": "Kasmā kāyā nu āgamma,",
  "vv34:2.4_0": "nāmena bhāsase maman”ti.",
  "vv34:3.1_0": "“Ahaṁ bhadde subhaddāsiṁ,",
  "vv34:3.2_0": "pubbe mānusake bhave;",
  "vv34:3.3_0": "Sahabhariyā ca te āsiṁ,",
  "vv34:3.4_0": "bhaginī ca kaniṭṭhikā.",
  "vv34:4.1_0": "Sā ahaṁ kāyassa bhedā,",
  "vv34:4.2_0": "vippamuttā tato cutā;",
  "vv34:4.3_0": "Nimmānaratīnaṁ devānaṁ,",
  "vv34:4.4_0": "upapannā sahabyatan”ti.",
  "vv34:5.1_0": "“Pahūtakatakalyāṇā,",
  "vv34:5.2_0": "te deve yanti pāṇino;",
  "vv34:5.3_0": "Yesaṁ tvaṁ kittayissasi,",
  "vv34:5.4_0": "subhadde jātimattano.",
  "vv34:6.1_0": "Atha tvaṁ kena vaṇṇena,",
  "vv34:6.2_0": "kena vā anusāsitā;",
  "vv34:6.3_0": "Kīdiseneva dānena,",
  "vv34:6.4_0": "subbatena yasassinī.",
  "vv34:7.1_0": "Yasaṁ etādisaṁ pattā,",
  "vv34:7.2_0": "visesaṁ vipulamajjhagā;",
  "vv34:7.3_0": "Devate pucchitācikkha,",
  "vv34:7.4_0": "kissa kammassidaṁ phalan”ti.",
  "vv34:8.1_0": "“Aṭṭheva piṇḍapātāni,",
  "vv34:8.2_0": "yaṁ dānaṁ adadaṁ pure;",
  "vv34:8.3_0": "Dakkhiṇeyyassa saṅghassa,",
  "vv34:8.4_0": "pasannā sehi pāṇibhi.",
  "vv34:9.1_0": "Tena metādiso vaṇṇo,",
  "vv34:9.2_0": "…pe…",
  "vv34:9.3_0": "vaṇṇo ca me sabbadisā pabhāsatī”ti.",
  "vv34:10.1_0": "“Ahaṁ tayā bahutare bhikkhū,",
  "vv34:10.2_0": "Saññate brahmacārayo;",
  "vv34:10.3_0": "Tappesiṁ annapānena,",
  "vv34:10.4_0": "Pasannā sehi pāṇibhi.",
  "vv34:11.1_0": "Tayā bahutaraṁ datvā,",
  "vv34:11.2_0": "hīnakāyūpagā ahaṁ;",
  "vv34:11.3_0": "Kathaṁ tvaṁ appataraṁ datvā,",
  "vv34:11.4_0": "visesaṁ vipulamajjhagā;",
  "vv34:11.5_0": "Devate pucchitācikkha,",
  "vv34:11.6_0": "kissa kammassidaṁ phalan”ti.",
  "vv34:12.1_0": "“Manobhāvanīyo bhikkhu,",
  "vv34:12.2_0": "sandiṭṭho me pure ahu;",
  "vv34:12.3_0": "Tāhaṁ bhattena nimantesiṁ,",
  "vv34:12.4_0": "revataṁ attanaṭṭhamaṁ.",
  "vv34:13.1_0": "So me atthapurekkhāro,",
  "vv34:13.2_0": "anukampāya revato;",
  "vv34:13.3_0": "Saṅghe dehīti maṁvoca,",
  "vv34:13.4_0": "tassāhaṁ vacanaṁ kariṁ.",
  "vv34:14.1_0": "Sā dakkhiṇā saṅghagatā,",
  "vv34:14.2_0": "appameyye patiṭṭhitā;",
  "vv34:14.3_0": "Puggalesu tayā dinnaṁ,",
  "vv34:14.4_0": "na taṁ tava mahapphalan”ti.",
  "vv34:15.1_0": "“Idānevāhaṁ jānāmi,",
  "vv34:15.2_0": "saṅghe dinnaṁ mahapphalaṁ;",
  "vv34:15.3_0": "Sāhaṁ gantvā manussattaṁ,",
  "vv34:15.4_0": "vadaññū vītamaccharā;",
  "vv34:15.5_0": "Saṅghe dānāni dassāmi,",
  "vv34:15.6_0": "appamattā punappunan”ti.",
  "vv34:16.1_0": "“Kā esā devatā bhadde,",
  "vv34:16.2_0": "tayā mantayate saha;",
  "vv34:16.3_0": "Sabbe deve tāvatiṁse,",
  "vv34:16.4_0": "vaṇṇena atirocatī”ti.",
  "vv34:17.1_0": "“Manussabhūtā devinda,",
  "vv34:17.2_0": "pubbe mānusake bhave;",
  "vv34:17.3_0": "Sahabhariyā ca me āsi,",
  "vv34:17.4_0": "bhaginī ca kaniṭṭhikā;",
  "vv34:17.5_0": "Saṅghe dānāni datvāna,",
  "vv34:17.6_0": "katapuññā virocatī”ti.",
  "vv34:18.1_0": "“Dhammena pubbe bhaginī,",
  "vv34:18.2_0": "tayā bhadde virocati;",
  "vv34:18.3_0": "Yaṁ saṅghamhi appameyye,",
  "vv34:18.4_0": "patiṭṭhāpesi dakkhiṇaṁ”.",
  "vv34:19.1_0": "“Pucchito hi mayā buddho,",
  "vv34:19.2_0": "gijjhakūṭamhi pabbate;",
  "vv34:19.3_0": "Vipākaṁ saṁvibhāgassa,",
  "vv34:19.4_0": "yattha dinnaṁ mahapphalaṁ.",
  "vv34:20.1_0": "Yajamānānaṁ manussānaṁ,",
  "vv34:20.2_0": "puññapekkhāna pāṇinaṁ;",
  "vv34:20.3_0": "Karotaṁ opadhikaṁ puññaṁ,",
  "vv34:20.4_0": "yattha dinnaṁ mahapphalaṁ.",
  "vv34:21.1_0": "Taṁ me buddho viyākāsi,",
  "vv34:21.2_0": "jānaṁ kammaphalaṁ sakaṁ;",
  "vv34:21.3_0": "Vipākaṁ saṁvibhāgassa,",
  "vv34:21.4_0": "yattha dinnaṁ mahapphalaṁ.",
  "vv34:22.1_0": "Cattāro ca paṭipannā,",
  "vv34:22.2_0": "cattāro ca phale ṭhitā;",
  "vv34:22.3_0": "Esa saṅgho ujubhūto,",
  "vv34:22.4_0": "paññāsīlasamāhito.",
  "vv34:23.1_0": "Yajamānānaṁ manussānaṁ,",
  "vv34:23.2_0": "puññapekkhāna pāṇinaṁ;",
  "vv34:23.3_0": "Karotaṁ opadhikaṁ puññaṁ,",
  "vv34:23.4_0": "saṅghe dinnaṁ mahapphalaṁ.",
  "vv34:24.1_0": "Eso hi saṅgho vipulo mahaggato,",
  "vv34:24.2_0": "Esappameyyo udadhīva sāgaro;",
  "vv34:24.3_0": "Ete hi seṭṭhā naravīrasāvakā,",
  "vv34:24.4_0": "Pabhaṅkarā dhammamudīrayanti.",
  "vv34:25.1_0": "Tesaṁ sudinnaṁ suhutaṁ suyiṭṭhaṁ,",
  "vv34:25.2_0": "Ye saṅghamuddissa dadanti dānaṁ;",
  "vv34:25.3_0": "Sā dakkhiṇā saṅghagatā patiṭṭhitā,",
  "vv34:25.4_0": "Mahapphalā lokavidūna vaṇṇitā.",
  "vv34:26.1_0": "Etādisaṁ yaññamanussarantā,",
  "vv34:26.2_0": "Ye vedajātā vicaranti loke;",
  "vv34:26.3_0": "Vineyya maccheramalaṁ samūlaṁ,",
  "vv34:26.4_0": "Aninditā saggamupenti ṭhānan”ti.",
  "vv34:27.1_0": "Daddallavimānaṁ chaṭṭhaṁ."
}