{
  "thag1.42:0.1_0": "Theragāthā 1.42",
  "thag1.42:0.2_0": "Ekakanipāta",
  "thag1.42:0.3_0": "Pañcamavagga",
  "thag1.42:0.4_0": "Khadiravaniyattheragāthā",
  "thag1.42:1.1_0": "“Cāle upacāle sīsūpacāle (…),",
