{
  "patthana1.18:0.1_0": "Paṭṭhānapakaraṇa",
  "patthana1.18:0.2_0": "Dhammānuloma (1), Tikapaṭṭhānapāḷi (1)",
  "patthana1.18:0.3_0": "1.18. Uppannattika",
  "patthana1.18:1.0.1_0": "Vibhaṅgavāra",
  "patthana1.18:1.0.2_0": "Hetu",
  "patthana1.18:1.1_0": "Uppanno dhammo uppannassa dhammassa hetupaccayena paccayo—uppannā hetū sampayuttakānaṁ khandhānaṁ cittasamuṭṭhānānañca rūpānaṁ hetupaccayena paccayo.",
  "patthana1.18:1.1_1": "Paṭisandhikkhaṇe uppannā hetū sampayuttakānaṁ khandhānaṁ kaṭattā ca rūpānaṁ hetupaccayena paccayo.",
  "patthana1.18:2.0_0": "Ārammaṇa",
  "patthana1.18:2.1_0": "Uppanno dhammo uppannassa dhammassa ārammaṇapaccayena paccayo—uppannaṁ cakkhuṁ aniccato dukkhato anattato vipassati,",
  "patthana1.18:2.1_1": "assādeti abhinandati,",
  "patthana1.18:2.1_2": "taṁ ārabbha rāgo uppajjati,",
  "patthana1.18:2.1_3": "diṭṭhi uppajjati,",
  "patthana1.18:2.1_4": "vicikicchā …pe…",
  "patthana1.18:2.1_5": "uddhaccaṁ …pe…",
  "patthana1.18:2.1_6": "domanassaṁ uppajjati.",
  "patthana1.18:2.1_7": "Uppannaṁ sotaṁ … ghānaṁ … jivhaṁ … kāyaṁ … rūpe … sadde … gandhe … rase …",
  "patthana1.18:2.1_8": "phoṭṭhabbe … vatthuṁ …",
  "patthana1.18:2.1_9": "uppanne khandhe aniccato dukkhato anattato vipassati …pe…",
  "patthana1.18:2.1_10": "domanassaṁ uppajjati.",
  "patthana1.18:2.1_11": "Dibbena cakkhunā rūpaṁ passati,",
  "patthana1.18:2.1_12": "dibbāya sotadhātuyā saddaṁ suṇāti,",
  "patthana1.18:2.1_13": "rūpāyatanaṁ cakkhuviññāṇassa …pe…",
  "patthana1.18:2.1_14": "phoṭṭhabbāyatanaṁ kāyaviññāṇassa …pe…",
  "patthana1.18:2.1_15": "uppannā khandhā iddhividhañāṇassa,",
  "patthana1.18:2.1_16": "āvajjanāya ārammaṇapaccayena paccayo.",
  "patthana1.18:3.1_0": "Anuppanno dhammo uppannassa dhammassa ārammaṇapaccayena paccayo—anuppanne rūpe … sadde … gandhe … rase …",
  "patthana1.18:3.1_1": "phoṭṭhabbe …",
  "patthana1.18:3.1_2": "anuppanne khandhe aniccato dukkhato anattato vipassati …pe…",
  "patthana1.18:3.1_3": "domanassaṁ uppajjati.",
  "patthana1.18:3.1_4": "Anuppannā khandhā iddhividhañāṇassa,",
  "patthana1.18:3.1_5": "cetopariyañāṇassa,",
  "patthana1.18:3.1_6": "anāgataṁsañāṇassa,",
  "patthana1.18:3.1_7": "āvajjanāya ārammaṇapaccayena paccayo.",
  "patthana1.18:4.1_0": "Uppādī dhammo uppannassa dhammassa ārammaṇapaccayena paccayo—uppādiṁ cakkhuṁ …pe… kāyaṁ … rūpe … gandhe … rase …",
  "patthana1.18:4.1_1": "phoṭṭhabbe … vatthuṁ …",
  "patthana1.18:4.1_2": "uppādī khandhe aniccato dukkhato anattato …pe…",
  "patthana1.18:4.1_3": "domanassaṁ uppajjati.",
  "patthana1.18:4.1_4": "Uppādī khandhā iddhividhañāṇassa,",
  "patthana1.18:4.1_5": "cetopariyañāṇassa,",
  "patthana1.18:4.1_6": "anāgataṁsañāṇassa,",
  "patthana1.18:4.1_7": "āvajjanāya ārammaṇapaccayena paccayo.",
  "patthana1.18:5.0_0": "Adhipati",
  "patthana1.18:5.1_0": "Uppanno dhammo uppannassa dhammassa adhipatipaccayena paccayo— ārammaṇādhipati,",
  "patthana1.18:5.1_1": "sahajātādhipati.",
  "patthana1.18:5.1_2": "**Ārammaṇādhipati**—uppannaṁ cakkhuṁ garuṁ katvā assādeti abhinandati,",
  "patthana1.18:5.1_3": "taṁ garuṁ katvā rāgo uppajjati,",
  "patthana1.18:5.1_4": "diṭṭhi uppajjati.",
  "patthana1.18:5.1_5": "Uppannaṁ sotaṁ … ghānaṁ … jivhaṁ … kāyaṁ … rūpe … sadde … gandhe … rase …",
  "patthana1.18:5.1_6": "phoṭṭhabbe … vatthuṁ …",
  "patthana1.18:5.1_7": "uppanne khandhe garuṁ katvā assādeti abhinandati,",
  "patthana1.18:5.1_8": "taṁ garuṁ katvā rāgo uppajjati …pe….",
  "patthana1.18:5.1_9": "**Sahajātādhipati**—uppannā adhipati sampayuttakānaṁ khandhānaṁ cittasamuṭṭhānānañca rūpānaṁ adhipatipaccayena paccayo.",
  "patthana1.18:6.1_0": "Anuppanno dhammo uppannassa dhammassa adhipatipaccayena paccayo.",
  "patthana1.18:6.1_1": "**Ārammaṇādhipati**—anuppanne rūpe … sadde … gandhe … rase …",
  "patthana1.18:6.1_2": "phoṭṭhabbe …",
  "patthana1.18:6.1_3": "anuppanne khandhe garuṁ katvā assādeti abhinandati,",
  "patthana1.18:6.1_4": "taṁ garuṁ katvā rāgo uppajjati,",
  "patthana1.18:6.1_5": "diṭṭhi uppajjati.",
  "patthana1.18:7.1_0": "Uppādī dhammo uppannassa dhammassa adhipatipaccayena paccayo.",
  "patthana1.18:7.1_1": "**Ārammaṇādhipati**—uppādiṁ cakkhuṁ …pe… kāyaṁ … rūpe …pe…",
  "patthana1.18:7.1_2": "phoṭṭhabbe … vatthuṁ …",
  "patthana1.18:7.1_3": "uppādī khandhe garuṁ katvā assādeti abhinandati,",
  "patthana1.18:7.1_4": "taṁ garuṁ katvā rāgo uppajjati,",
  "patthana1.18:7.1_5": "diṭṭhi uppajjati.",
  "patthana1.18:8.0_0": "Sahajāta",
  "patthana1.18:8.1_0": "Uppanno dhammo uppannassa dhammassa sahajātapaccayena paccayo—uppanno eko khandho tiṇṇannaṁ khandhānaṁ",
  "patthana1.18:8.1_1": "cittasamuṭṭhānānañca rūpānaṁ sahajātapaccayena paccayo …pe…",
  "patthana1.18:8.1_2": "dve khandhā dvinnaṁ khandhānaṁ cittasamuṭṭhānānañca rūpānaṁ sahajātapaccayena paccayo.",
  "patthana1.18:8.1_3": "Paṭisandhikkhaṇe uppanno eko khandho tiṇṇannaṁ khandhānaṁ kaṭattā ca rūpānaṁ sahajātapaccayena paccayo …pe…",
  "patthana1.18:8.1_4": "dve khandhā dvinnaṁ khandhānaṁ kaṭattā ca rūpānaṁ sahajātapaccayena paccayo.",
  "patthana1.18:8.1_5": "Khandhā vatthussa sahajātapaccayena paccayo.",
  "patthana1.18:8.1_6": "Vatthu khandhānaṁ sahajātapaccayena paccayo.",
  "patthana1.18:8.1_7": "Ekaṁ mahābhūtaṁ tiṇṇannaṁ mahābhūtānaṁ sahajātapaccayena paccayo …pe…",
  "patthana1.18:8.1_8": "dve mahābhūtā …pe…",
  "patthana1.18:8.1_9": "mahābhūtā cittasamuṭṭhānānaṁ rūpānaṁ kaṭattārūpānaṁ upādārūpānaṁ sahajātapaccayena paccayo. bāhiraṁ …",
  "patthana1.18:8.1_10": "āhārasamuṭṭhānaṁ …",
  "patthana1.18:8.1_11": "utusamuṭṭhānaṁ …",
  "patthana1.18:8.1_12": "asaññasattānaṁ ekaṁ mahābhūtaṁ …pe…",
  "patthana1.18:8.1_13": "dve mahābhūtā …pe…",
  "patthana1.18:8.1_14": "mahābhūtā kaṭattārūpānaṁ upādārūpānaṁ sahajātapaccayena paccayo.",
  "patthana1.18:9.0_0": "Aññamañña",
  "patthana1.18:9.1_0": "Uppanno dhammo uppannassa dhammassa aññamaññapaccayena paccayo—uppanno eko khandho tiṇṇannaṁ khandhānaṁ aññamaññapaccayena paccayo …pe…",
  "patthana1.18:9.1_1": "dve khandhā …pe…",
  "patthana1.18:9.1_2": "paṭisandhikkhaṇe uppanno eko khandho tiṇṇannaṁ khandhānaṁ vatthussa ca aññamaññapaccayena paccayo.",
  "patthana1.18:9.1_3": "Dve khandhā …pe…",
  "patthana1.18:9.1_4": "khandhā vatthussa aññamaññapaccayena paccayo.",
  "patthana1.18:9.1_5": "Vatthu khandhānaṁ aññamaññapaccayena paccayo.",
  "patthana1.18:9.1_6": "Ekaṁ mahābhūtaṁ …pe… bāhiraṁ …",
  "patthana1.18:9.1_7": "āhārasamuṭṭhānaṁ …",
  "patthana1.18:9.1_8": "utusamuṭṭhānaṁ …",
  "patthana1.18:9.1_9": "asaññasattānaṁ ekaṁ mahābhūtaṁ tiṇṇannaṁ mahābhūtānaṁ aññamaññapaccayena paccayo …pe…",
  "patthana1.18:9.1_10": "dve mahābhūtā …pe….",
  "patthana1.18:10.0_0": "Nissaya",
  "patthana1.18:10.1_0": "Uppanno dhammo uppannassa dhammassa nissayapaccayena paccayo—uppanno eko khandho tiṇṇannaṁ khandhānaṁ",
  "patthana1.18:10.1_1": "cittasamuṭṭhānānañca rūpānaṁ nissayapaccayena paccayo …pe…",
  "patthana1.18:10.1_2": "dve khandhā …pe…",
  "patthana1.18:10.1_3": "paṭisandhikkhaṇe …pe…",
  "patthana1.18:10.1_4": "khandhā vatthussa …pe…",
  "patthana1.18:10.1_5": "vatthu khandhānaṁ …pe…",
  "patthana1.18:10.1_6": "ekaṁ mahābhūtaṁ …pe… bāhiraṁ …",
  "patthana1.18:10.1_7": "āhārasamuṭṭhānaṁ …",
  "patthana1.18:10.1_8": "utusamuṭṭhānaṁ …",
  "patthana1.18:10.1_9": "asaññasattānaṁ ekaṁ mahābhūtaṁ …pe…",
  "patthana1.18:10.1_10": "mahābhūtā cittasamuṭṭhānānaṁ rūpānaṁ kaṭattārūpānaṁ upādārūpānaṁ cakkhāyatanaṁ cakkhuviññāṇassa …pe…",
  "patthana1.18:10.1_11": "kāyāyatanaṁ kāyaviññāṇassa …pe…",
  "patthana1.18:10.1_12": "vatthu uppannānaṁ khandhānaṁ nissayapaccayena paccayo.",
  "patthana1.18:11.0_0": "Upanissaya",
  "patthana1.18:11.1_0": "Uppanno dhammo uppannassa dhammassa upanissayapaccayena paccayo— ārammaṇūpanissayo,",
  "patthana1.18:11.1_1": "pakatūpanissayo …pe….",
  "patthana1.18:11.1_2": "**Pakatūpanissayo**—uppannaṁ utuṁ upanissāya jhānaṁ uppādeti,",
  "patthana1.18:11.1_3": "vipassanaṁ …pe…",
  "patthana1.18:11.1_4": "maggaṁ …pe…",
  "patthana1.18:11.1_5": "abhiññaṁ …pe…",
  "patthana1.18:11.1_6": "samāpattiṁ uppādeti,",
  "patthana1.18:11.1_7": "mānaṁ jappeti,",
  "patthana1.18:11.1_8": "diṭṭhiṁ gaṇhāti.",
  "patthana1.18:11.1_9": "Uppannaṁ bhojanaṁ …pe…",
  "patthana1.18:11.1_10": "senāsanaṁ upanissāya jhānaṁ uppādeti vipassanaṁ …pe…",
  "patthana1.18:11.1_11": "maggaṁ …pe…",
  "patthana1.18:11.1_12": "abhiññaṁ …pe…",
  "patthana1.18:11.1_13": "samāpattiṁ uppādeti,",
  "patthana1.18:11.1_14": "mānaṁ jappeti,",
  "patthana1.18:11.1_15": "diṭṭhiṁ gaṇhāti.",
  "patthana1.18:11.1_16": "Uppannaṁ utu …",
  "patthana1.18:11.1_17": "bhojanaṁ …",
  "patthana1.18:11.1_18": "senāsanaṁ uppannāya saddhāya …pe… paññāya,",
  "patthana1.18:11.1_19": "kāyikassa sukhassa,",
  "patthana1.18:11.1_20": "kāyikassa dukkhassa, maggassa,",
  "patthana1.18:11.1_21": "phalasamāpattiyā upanissayapaccayena paccayo.",
  "patthana1.18:12.1_0": "Anuppanno dhammo uppannassa dhammassa upanissayapaccayena paccayo— ārammaṇūpanissayo,",
  "patthana1.18:12.1_1": "pakatūpanissayo …pe….",
  "patthana1.18:12.1_2": "**Pakatūpanissayo**—anuppannaṁ vaṇṇasampadaṁ patthayamāno dānaṁ deti,",
  "patthana1.18:12.1_3": "sīlaṁ samādiyati,",
  "patthana1.18:12.1_4": "uposathakammaṁ karoti.",
  "patthana1.18:12.1_5": "Anuppannaṁ saddasampadaṁ …",
  "patthana1.18:12.1_6": "gandhasampadaṁ …",
  "patthana1.18:12.1_7": "rasasampadaṁ …",
  "patthana1.18:12.1_8": "phoṭṭhabbasampadaṁ …",
  "patthana1.18:12.1_9": "anuppanne khandhe patthayamāno dānaṁ deti,",
  "patthana1.18:12.1_10": "sīlaṁ samādiyati,",
  "patthana1.18:12.1_11": "uposathakammaṁ karoti.",
  "patthana1.18:12.1_12": "Anuppannā vaṇṇasampadā …pe…",
  "patthana1.18:12.1_13": "anuppannā khandhā uppannāya saddhāya …pe… paññāya,",
  "patthana1.18:12.1_14": "kāyikassa sukhassa,",
  "patthana1.18:12.1_15": "kāyikassa dukkhassa, maggassa,",
  "patthana1.18:12.1_16": "phalasamāpattiyā upanissayapaccayena paccayo.",
  "patthana1.18:13.1_0": "Uppādī dhammo uppannassa dhammassa upanissayapaccayena paccayo— ārammaṇūpanissayo,",
  "patthana1.18:13.1_1": "pakatūpanissayo …pe….",
  "patthana1.18:13.1_2": "**Pakatūpanissayo**—uppādiṁ cakkhusampadaṁ patthayamāno dānaṁ deti,",
  "patthana1.18:13.1_3": "sīlaṁ samādiyati,",
  "patthana1.18:13.1_4": "uposathakammaṁ karoti.",
  "patthana1.18:13.1_5": "Uppādiṁ sotasampadaṁ …pe…",
  "patthana1.18:13.1_6": "kāyasampadaṁ …pe…",
  "patthana1.18:13.1_7": "vaṇṇasampadaṁ …",
  "patthana1.18:13.1_8": "gandhasampadaṁ …",
  "patthana1.18:13.1_9": "rasasampadaṁ …",
  "patthana1.18:13.1_10": "phoṭṭhabbasampadaṁ …",
  "patthana1.18:13.1_11": "uppādī khandhe patthayamāno dānaṁ deti,",
  "patthana1.18:13.1_12": "sīlaṁ samādiyati,",
  "patthana1.18:13.1_13": "uposathakammaṁ karoti.",
  "patthana1.18:13.1_14": "Uppādī cakkhusampadā …pe…",
  "patthana1.18:13.1_15": "kāyasampadā …",
  "patthana1.18:13.1_16": "vaṇṇasampadā …pe…",
  "patthana1.18:13.1_17": "phoṭṭhabbasampadā …",
  "patthana1.18:13.1_18": "uppādī khandhā uppannāya saddhāya …pe… paññāya,",
  "patthana1.18:13.1_19": "kāyikassa sukhassa,",
  "patthana1.18:13.1_20": "kāyikassa dukkhassa, maggassa,",
  "patthana1.18:13.1_21": "phalasamāpattiyā upanissayapaccayena paccayo.",
  "patthana1.18:14.0_0": "Purejāta",
  "patthana1.18:14.1_0": "Uppanno dhammo uppannassa dhammassa purejātapaccayena paccayo— ārammaṇapurejātaṁ,",
  "patthana1.18:14.1_1": "vatthupurejātaṁ.",
  "patthana1.18:14.1_2": "**Ārammaṇapurejātaṁ**—cakkhuṁ …pe…",
  "patthana1.18:14.1_3": "vatthuṁ aniccato dukkhato anattato vipassati,",
  "patthana1.18:14.1_4": "assādeti abhinandati,",
  "patthana1.18:14.1_5": "taṁ ārabbha rāgo uppajjati …pe…",
  "patthana1.18:14.1_6": "domanassaṁ uppajjati dibbena cakkhunā rūpaṁ passati,",
  "patthana1.18:14.1_7": "dibbāya sotadhātuyā saddaṁ suṇāti,",
  "patthana1.18:14.1_8": "rūpāyatanaṁ cakkhuviññāṇassa …pe…",
  "patthana1.18:14.1_9": "phoṭṭhabbāyatanaṁ kāyaviññāṇassa purejātapaccayena paccayo.",
  "patthana1.18:14.1_10": "**Vatthupurejātaṁ**—cakkhāyatanaṁ cakkhuviññāṇassa …pe…",
  "patthana1.18:14.1_11": "vatthu uppannānaṁ khandhānaṁ purejātapaccayena paccayo.",
  "patthana1.18:15.0_0": "Pacchājāta",
  "patthana1.18:15.1_0": "Uppanno dhammo uppannassa dhammassa pacchājātapaccayena paccayo—pacchājātā uppannā khandhā purejātassa imassa kāyassa pacchājātapaccayena paccayo.",
  "patthana1.18:16.0_0": "Kamma",
  "patthana1.18:16.1_0": "Uppanno dhammo uppannassa dhammassa kammapaccayena paccayo—uppannā cetanā sampayuttakānaṁ khandhānaṁ",
  "patthana1.18:16.1_1": "cittasamuṭṭhānānañca rūpānaṁ kammapaccayena paccayo.",
  "patthana1.18:16.1_2": "Paṭisandhikkhaṇe uppannā cetanā sampayuttakānaṁ khandhānaṁ kaṭattā ca rūpānaṁ kammapaccayena paccayo.",
  "patthana1.18:17.0_0": "Vipāka",
  "patthana1.18:17.1_0": "Uppanno dhammo uppannassa dhammassa vipākapaccayena paccayo—vipāko uppanno eko khandho tiṇṇannaṁ khandhānaṁ",
  "patthana1.18:17.1_1": "cittasamuṭṭhānānañca rūpānaṁ vipākapaccayena paccayo …pe…",
  "patthana1.18:17.1_2": "dve khandhā …pe…",
  "patthana1.18:17.1_3": "paṭisandhikkhaṇe uppanno eko khandho tiṇṇannaṁ khandhānaṁ kaṭattā ca rūpānaṁ …pe…",
  "patthana1.18:17.1_4": "dve khandhā …pe…",
  "patthana1.18:17.1_5": "khandhā vatthussa vipākapaccayena paccayo.",
  "patthana1.18:18.0_0": "Āhāra",
  "patthana1.18:18.1_0": "Uppanno dhammo uppannassa dhammassa āhārapaccayena paccayo—uppannā āhārā sampayuttakānaṁ khandhānaṁ cittasamuṭṭhānānañca",
  "patthana1.18:18.1_1": "rūpānaṁ āhārapaccayena paccayo.",
  "patthana1.18:18.1_2": "Paṭisandhikkhaṇe …pe….",
  "patthana1.18:18.1_3": "**Kabaḷīkāro āhāro**—imassa kāyassa āhārapaccayena paccayo.",
  "patthana1.18:19.0_0": "Indriya",
  "patthana1.18:19.1_0": "Uppanno dhammo uppannassa dhammassa indriyapaccayena paccayo—uppannā indriyā sampayuttakānaṁ khandhānaṁ",
  "patthana1.18:19.1_1": "cittasamuṭṭhānānañca rūpānaṁ indriyapaccayena paccayo.",
  "patthana1.18:19.1_2": "Paṭisandhikkhaṇe …pe…",
  "patthana1.18:19.1_3": "cakkhundriyaṁ cakkhuviññāṇassa …pe…",
  "patthana1.18:19.1_4": "kāyindriyaṁ kāyaviññāṇassa …pe…",
  "patthana1.18:19.1_5": "rūpajīvitindriyaṁ kaṭattārūpānaṁ indriyapaccayena paccayo.",
  "patthana1.18:20.0_0": "Jhānādi",
  "patthana1.18:20.1_0": "Uppanno dhammo uppannassa dhammassa jhānapaccayena paccayo …",
  "patthana1.18:20.1_1": "maggapaccayena paccayo …",
  "patthana1.18:20.1_2": "sampayuttapaccayena paccayo …",
  "patthana1.18:20.1_3": "vippayuttapaccayena paccayo— sahajātaṁ,",
  "patthana1.18:20.1_4": "purejātaṁ,",
  "patthana1.18:20.1_5": "pacchājātaṁ.",
  "patthana1.18:20.1_6": "**Sahajātā**—uppannā khandhā cittasamuṭṭhānānaṁ rūpānaṁ vippayuttapaccayena paccayo.",
  "patthana1.18:20.1_7": "Paṭisandhikkhaṇe uppannā khandhā kaṭattārūpānaṁ vippayuttapaccayena paccayo.",
  "patthana1.18:20.1_8": "Khandhā vatthussa,",
  "patthana1.18:20.1_9": "vatthu khandhānaṁ vippayuttapaccayena paccayo.",
  "patthana1.18:20.1_10": "**Purejātaṁ**—cakkhāyatanaṁ cakkhuviññāṇassa …pe…",
  "patthana1.18:20.1_11": "kāyāyatanaṁ kāyaviññāṇassa …",
  "patthana1.18:20.1_12": "vatthu uppannānaṁ khandhānaṁ vippayuttapaccayena paccayo.",
  "patthana1.18:20.1_13": "**Pacchājātā**—uppannā khandhā purejātassa imassa kāyassa vippayuttapaccayena paccayo.",
  "patthana1.18:21.0_0": "Atthi",
  "patthana1.18:21.1_0": "Uppanno dhammo uppannassa dhammassa atthipaccayena paccayo—sahajātaṁ,",
  "patthana1.18:21.1_1": "purejātaṁ,",
  "patthana1.18:21.1_2": "pacchājātaṁ, āhāraṁ, indriyaṁ.",
  "patthana1.18:21.1_3": "**Sahajāto**—uppanno eko khandho tiṇṇannaṁ khandhānaṁ cittasamuṭṭhānānañca rūpānaṁ atthipaccayena paccayo …pe…",
  "patthana1.18:21.1_4": "dve khandhā …pe…",
  "patthana1.18:21.1_5": "paṭisandhikkhaṇe …pe…",
  "patthana1.18:21.1_6": "ekaṁ mahābhūtaṁ …pe… bāhiraṁ …",
  "patthana1.18:21.1_7": "āhārasamuṭṭhānaṁ …",
  "patthana1.18:21.1_8": "utusamuṭṭhānaṁ …",
  "patthana1.18:21.1_9": "asaññasattānaṁ …pe….",
  "patthana1.18:21.1_10": "**Purejātaṁ**—cakkhuṁ aniccato …pe…",
  "patthana1.18:21.1_11": "vatthuṁ aniccato …pe…",
  "patthana1.18:21.1_12": "domanassaṁ uppajjati,",
  "patthana1.18:21.1_13": "dibbena cakkhunā rūpaṁ passati,",
  "patthana1.18:21.1_14": "dibbāya sotudhātuyā saddaṁ suṇāti,",
  "patthana1.18:21.1_15": "rūpāyatanaṁ cakkhuviññāṇassa …pe…",
  "patthana1.18:21.1_16": "phoṭṭhabbāyatanaṁ kāyaviññāṇassa atthipaccayena paccayo.",
  "patthana1.18:21.1_17": "Cakkhāyatanaṁ cakkhuviññāṇassa …pe…",
  "patthana1.18:21.1_18": "kāyāyatanaṁ kāyaviññāṇassa …pe…",
  "patthana1.18:21.1_19": "vatthu uppannānaṁ khandhānaṁ atthipaccayena paccayo.",
  "patthana1.18:21.1_20": "**Pacchājātā**—uppannā khandhā purejātassa imassa kāyassa atthipaccayena paccayo.",
  "patthana1.18:21.1_21": "**Kabaḷīkāro āhāro**—imassa kāyassa atthipaccayena paccayo.",
  "patthana1.18:21.1_22": "**Rūpajīvitindriyaṁ**—kaṭattārūpānaṁ atthipaccayena paccayo.",
  "patthana1.18:22.0_0": "Avigata",
  "patthana1.18:22.1_0": "Uppanno dhammo uppannassa dhammassa avigatapaccayena paccayo …pe….",
  "patthana1.18:23.0.1_0": "Saṅkhyāvāra",
  "patthana1.18:23.0.2_0": "Suddha",
  "patthana1.18:23.1_0": "Hetuyā ekaṁ,",
  "patthana1.18:23.1_1": "ārammaṇe tīṇi,",
  "patthana1.18:23.1_2": "adhipatiyā tīṇi,",
  "patthana1.18:23.1_3": "sahajāte ekaṁ,",
  "patthana1.18:23.1_4": "aññamaññe ekaṁ,",
  "patthana1.18:23.1_5": "nissaye ekaṁ,",
  "patthana1.18:23.1_6": "upanissaye tīṇi,",
  "patthana1.18:23.1_7": "purejāte ekaṁ,",
  "patthana1.18:23.1_8": "pacchājāte kamme vipāke āhāre indriye jhāne magge sampayutte vippayutte atthiyā avigate ekaṁ.",
  "patthana1.18:23.1_9": "(Evaṁ gaṇetabbaṁ.)",
  "patthana1.18:24.1_0": "Anulomaṁ.",
  "patthana1.18:25.0_0": "Paccanīyuddhāra",
  "patthana1.18:25.1_0": "Uppanno dhammo uppannassa dhammassa ārammaṇapaccayena paccayo …",
  "patthana1.18:25.1_1": "sahajātapaccayena paccayo …",
  "patthana1.18:25.1_2": "upanissayapaccayena paccayo …",
  "patthana1.18:25.1_3": "purejātapaccayena paccayo …",
  "patthana1.18:25.1_4": "pacchājātapaccayena paccayo …",
  "patthana1.18:25.1_5": "āhārapaccayena paccayo …",
  "patthana1.18:25.1_6": "indriyapaccayena paccayo.",
  "patthana1.18:26.1_0": "Anuppanno dhammo uppannassa dhammassa ārammaṇapaccayena paccayo, upanissayapaccayena paccayo.",
  "patthana1.18:27.1_0": "Uppādī dhammo uppannassa dhammassa ārammaṇapaccayena paccayo … upanissayapaccayena paccayo.",
  "patthana1.18:28.0.1_0": "1.18.7.2. Paccayapaccanīya",
  "patthana1.18:28.0.2_0": "Saṅkhyāvāra",
  "patthana1.18:28.0.3_0": "Suddha",
  "patthana1.18:28.1_0": "Nahetuyā tīṇi,",
  "patthana1.18:28.1_1": "naārammaṇe tīṇi,",
  "patthana1.18:28.1_2": "naadhipatiyā tīṇi …pe…",
  "patthana1.18:28.1_3": "navippayutte tīṇi,",
  "patthana1.18:28.1_4": "noatthiyā dve,",
  "patthana1.18:28.1_5": "nonatthiyā tīṇi,",
  "patthana1.18:28.1_6": "novigate tīṇi,",
  "patthana1.18:28.1_7": "noavigate dve.",
  "patthana1.18:28.1_8": "(Evaṁ gaṇetabbaṁ.)",
  "patthana1.18:29.1_0": "Paccanīyaṁ.",
  "patthana1.18:30.0_0": "1.18.7.3. Paccayānulomapaccanīya",
  "patthana1.18:30.1_0": "Hetupaccayā naārammaṇe ekaṁ …pe… nonatthiyā novigate ekaṁ.",
  "patthana1.18:31.1_0": "Anulomapaccanīyaṁ.",
  "patthana1.18:32.0_0": "1.18.7.4. Paccanīyānuloma",
  "patthana1.18:32.1_0": "Nahetupaccayā ārammaṇe tīṇi,",
  "patthana1.18:32.1_1": "adhipatiyā tīṇi,",
  "patthana1.18:32.1_2": "sahajāte ekaṁ,",
  "patthana1.18:32.1_3": "aññamaññe ekaṁ,",
  "patthana1.18:32.1_4": "nissaye ekaṁ,",
  "patthana1.18:32.1_5": "upanissaye tīṇi,",
  "patthana1.18:32.1_6": "purejāte ekaṁ,",
  "patthana1.18:32.1_7": "pacchājāte ekaṁ,",
  "patthana1.18:32.1_8": "kamme vipāke āhāre indriye jhāne magge sampayutte vippayutte atthiyā avigate ekaṁ.",
  "patthana1.18:33.1_0": "Paccanīyānulomaṁ.",
  "patthana1.18:34.1_0": "Uppannattikaṁ niṭṭhitaṁ."
}