{
  "thag2.37:0.1_0": "Theragāthā 2.37",
  "thag2.37:0.2_0": "Dukanipāta",
  "thag2.37:0.3_0": "Catutthavagga",
  "thag2.37:0.4_0": "Soṇapoṭiriyattheragāthā",
  "thag2.37:1.1_0": "“Na tāva supituṁ hoti,",
  "thag2.37:1.2_0": "ratti nakkhattamālinī;",
  "thag2.37:1.3_0": "Paṭijaggitumevesā,",
  "thag2.37:1.4_0": "ratti hoti vijānatā”.",
  "thag2.37:2.1_0": "“Hatthikkhandhāvapatitaṁ,",
  "thag2.37:2.2_0": "kuñjaro ce anukkame;",
  "thag2.37:2.3_0": "Saṅgāme me mataṁ seyyo,",
  "thag2.37:2.4_0": "yañce jīve parājito”ti.",
  "thag2.37:2.5_0": "… Soṇo poṭiriyo thero …."
}