{
  "thag4.11:0.1_0": "Theragāthā 4.11",
  "thag4.11:0.2_0": "Catukkanipāta",
  "thag4.11:0.3_0": "Paṭhamavagga",
  "thag4.11:0.4_0": "Sappakattheragāthā",
  "thag4.11:1.1_0": "“Yadā balākā sucipaṇḍaracchadā,",
  "thag4.11:1.2_0": "Kāḷassa meghassa bhayena tajjitā;",
  "thag4.11:1.3_0": "Palehiti ālayamālayesinī,",
  "thag4.11:1.4_0": "Tadā nadī ajakaraṇī rameti maṁ.",
  "thag4.11:2.1_0": "Yadā balākā suvisuddhapaṇḍarā,",
  "thag4.11:2.2_0": "Kāḷassa meghassa bhayena tajjitā;",
  "thag4.11:2.3_0": "Pariyesati leṇamaleṇadassinī,",
  "thag4.11:2.4_0": "Tadā nadī ajakaraṇī rameti maṁ.",
  "thag4.11:3.1_0": "Kaṁ nu tattha na ramenti,",
  "thag4.11:3.2_0": "jambuyo ubhato tahiṁ;",
  "thag4.11:3.3_0": "Sobhenti āpagākūlaṁ,",
  "thag4.11:3.4_0": "mama leṇassa pacchato.",
  "thag4.11:4.1_0": "Tāmatamadasaṅghasuppahīnā,",
  "thag4.11:4.2_0": "Bhekā mandavatī panādayanti;",
  "thag4.11:4.3_0": "‘Nājja girinadīhi vippavāsasamayo,",
  "thag4.11:4.4_0": "Khemā ajakaraṇī sivā surammā’”ti.",
  "thag4.11:4.5_0": "… Sappako thero …."
}