{
  "thag1.114:0.1_0": "Theragāthā 1.114",
  "thag1.114:0.2_0": "Ekakanipāta",
  "thag1.114:0.3_0": "Dvādasamavagga",
  "thag1.114:0.4_0": "Adhimuttattheragāthā",
  "thag1.114:1.1_0": "“Kāyaduṭṭhullagaruno,",
  "thag1.114:1.2_0": "hiyyamānamhi jīvite;",
  "thag1.114:1.3_0": "Sarīrasukhagiddhassa,",
  "thag1.114:1.4_0": "kuto samaṇasādhutā”ti.",
  "thag1.114:1.5_0": "… Adhimutto thero …."
}