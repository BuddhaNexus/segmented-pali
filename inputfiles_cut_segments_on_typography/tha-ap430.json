{
  "tha-ap430:0.1_0": "Therāpadāna",
  "tha-ap430:0.2_0": "Sakiṁsammajjakavagga",
  "tha-ap430:0.3_0": "8. Sucintitattheraapadāna",
  "tha-ap430:1.1_0": "“Nagare haṁsavatiyā,",
  "tha-ap430:1.2_0": "ahosiṁ kassako tadā;",
  "tha-ap430:1.3_0": "Kasikammena jīvāmi,",
  "tha-ap430:1.4_0": "tena posemi dārake.",
  "tha-ap430:2.1_0": "Susampannaṁ tadā khettaṁ,",
  "tha-ap430:2.2_0": "Dhaññaṁ me phalinaṁ ahu;",
  "tha-ap430:2.3_0": "Pākakāle ca sampatte,",
  "tha-ap430:2.4_0": "Evaṁ cintesahaṁ tadā.",
  "tha-ap430:3.1_0": "Nacchannaṁ nappatirūpaṁ,",
  "tha-ap430:3.2_0": "jānantassa guṇāguṇaṁ;",
  "tha-ap430:3.3_0": "Yohaṁ saṅghe adatvāna,",
  "tha-ap430:3.4_0": "aggaṁ bhuñjeyya ce tadā.",
  "tha-ap430:4.1_0": "Ayaṁ buddho asamasamo,",
  "tha-ap430:4.2_0": "dvattiṁsavaralakkhaṇo;",
  "tha-ap430:4.3_0": "Tato pabhāvito saṅgho,",
  "tha-ap430:4.4_0": "puññakkhetto anuttaro.",
  "tha-ap430:5.1_0": "Tattha dassāmahaṁ dānaṁ,",
  "tha-ap430:5.2_0": "navasassaṁ pure pure;",
  "tha-ap430:5.3_0": "Evāhaṁ cintayitvāna,",
  "tha-ap430:5.4_0": "haṭṭho pīṇitamānaso.",
  "tha-ap430:6.1_0": "Khettato dhaññamāhatvā,",
  "tha-ap430:6.2_0": "sambuddhaṁ upasaṅkamiṁ;",
  "tha-ap430:6.3_0": "Upasaṅkamma sambuddhaṁ,",
  "tha-ap430:6.4_0": "lokajeṭṭhaṁ narāsabhaṁ;",
  "tha-ap430:6.5_0": "Vanditvā satthuno pāde,",
  "tha-ap430:6.6_0": "idaṁ vacanamabraviṁ.",
  "tha-ap430:7.1_0": "‘Navasassañca sampannaṁ,",
  "tha-ap430:7.2_0": "āyāgosi ca tvaṁ mune;",
  "tha-ap430:7.3_0": "Anukampamupādāya,",
  "tha-ap430:7.4_0": "adhivāsehi cakkhuma’.",
  "tha-ap430:8.1_0": "Padumuttaro lokavidū,",
  "tha-ap430:8.2_0": "āhutīnaṁ paṭiggaho;",
  "tha-ap430:8.3_0": "Mama saṅkappamaññāya,",
  "tha-ap430:8.4_0": "idaṁ vacanamabravi.",
  "tha-ap430:9.1_0": "‘Cattāro ca paṭipannā,",
  "tha-ap430:9.2_0": "cattāro ca phale ṭhitā;",
  "tha-ap430:9.3_0": "Esa saṅgho ujubhūto,",
  "tha-ap430:9.4_0": "paññāsīlasamāhito;",
  "tha-ap430:9.5_0": "Yajantānaṁ manussānaṁ,",
  "tha-ap430:9.6_0": "puññapekkhāna pāṇinaṁ.",
  "tha-ap430:10.1_0": "Karotopadhikaṁ puññaṁ,",
  "tha-ap430:10.2_0": "saṅghe dinnaṁ mahapphalaṁ;",
  "tha-ap430:10.3_0": "Tasmiṁ saṅgheva dātabbaṁ,",
  "tha-ap430:10.4_0": "tava sassaṁ tathetaraṁ.",
  "tha-ap430:11.1_0": "Saṅghato uddisitvāna,",
  "tha-ap430:11.2_0": "bhikkhū netvāna saṅgharaṁ;",
  "tha-ap430:11.3_0": "Paṭiyattaṁ ghare santaṁ,",
  "tha-ap430:11.4_0": "bhikkhusaṅghassa dehi tvaṁ’.",
  "tha-ap430:12.1_0": "Saṅghato uddisitvāna,",
  "tha-ap430:12.2_0": "bhikkhū netvāna saṅgharaṁ;",
  "tha-ap430:12.3_0": "Yaṁ ghare paṭiyattaṁ me,",
  "tha-ap430:12.4_0": "bhikkhusaṅghassadāsahaṁ.",
  "tha-ap430:13.1_0": "Tena kammena sukatena,",
  "tha-ap430:13.2_0": "cetanāpaṇidhīhi ca;",
  "tha-ap430:13.3_0": "Jahitvā mānusaṁ dehaṁ,",
  "tha-ap430:13.4_0": "tāvatiṁsamagacchahaṁ.",
  "tha-ap430:14.1_0": "Tattha me sukataṁ byamhaṁ,",
  "tha-ap430:14.2_0": "sovaṇṇaṁ sappabhassaraṁ;",
  "tha-ap430:14.3_0": "Saṭṭhiyojanamubbedhaṁ,",
  "tha-ap430:14.4_0": "tiṁsayojanavitthataṁ.",
  "tha-ap430:15.1_0": "Ekūnavīsatimaṁ bhāṇavāraṁ.",
  "tha-ap430:16.1_0": "Ākiṇṇaṁ bhavanaṁ mayhaṁ,",
  "tha-ap430:16.2_0": "nārīgaṇasamākulaṁ;",
  "tha-ap430:16.3_0": "Tattha bhutvā pivitvā ca,",
  "tha-ap430:16.4_0": "vasāmi tidase ahaṁ.",
  "tha-ap430:17.1_0": "Satānaṁ tīṇikkhattuñca,",
  "tha-ap430:17.2_0": "devarajjamakārayiṁ;",
  "tha-ap430:17.3_0": "Satānaṁ pañcakkhattuñca,",
  "tha-ap430:17.4_0": "cakkavattī ahosahaṁ;",
  "tha-ap430:17.5_0": "Padesarajjaṁ vipulaṁ,",
  "tha-ap430:17.6_0": "gaṇanāto asaṅkhiyaṁ.",
  "tha-ap430:18.1_0": "Bhavābhave saṁsaranto,",
  "tha-ap430:18.2_0": "labhāmi amitaṁ dhanaṁ;",
  "tha-ap430:18.3_0": "Bhoge me ūnatā natthi,",
  "tha-ap430:18.4_0": "navasassassidaṁ phalaṁ.",
  "tha-ap430:19.1_0": "Hatthiyānaṁ assayānaṁ,",
  "tha-ap430:19.2_0": "sivikaṁ sandamānikaṁ;",
  "tha-ap430:19.3_0": "Labhāmi sabbamevetaṁ,",
  "tha-ap430:19.4_0": "navasassassidaṁ phalaṁ.",
  "tha-ap430:20.1_0": "Navavatthaṁ navaphalaṁ,",
  "tha-ap430:20.2_0": "navaggarasabhojanaṁ;",
  "tha-ap430:20.3_0": "Labhāmi sabbamevetaṁ,",
  "tha-ap430:20.4_0": "navasassassidaṁ phalaṁ.",
  "tha-ap430:21.1_0": "Koseyyakambaliyāni,",
  "tha-ap430:21.2_0": "khomakappāsikāni ca;",
  "tha-ap430:21.3_0": "Labhāmi sabbamevetaṁ,",
  "tha-ap430:21.4_0": "navasassassidaṁ phalaṁ.",
  "tha-ap430:22.1_0": "Dāsīgaṇaṁ dāsagaṇaṁ,",
  "tha-ap430:22.2_0": "nāriyo ca alaṅkatā;",
  "tha-ap430:22.3_0": "Labhāmi sabbamevetaṁ,",
  "tha-ap430:22.4_0": "navasassassidaṁ phalaṁ.",
  "tha-ap430:23.1_0": "Na maṁ sītaṁ vā uṇhaṁ vā,",
  "tha-ap430:23.2_0": "pariḷāho na vijjati;",
  "tha-ap430:23.3_0": "Atho cetasikaṁ dukkhaṁ,",
  "tha-ap430:23.4_0": "hadaye me na vijjati.",
  "tha-ap430:24.1_0": "Idaṁ khāda idaṁ bhuñja,",
  "tha-ap430:24.2_0": "imamhi sayane saya;",
  "tha-ap430:24.3_0": "Labhāmi sabbamevetaṁ,",
  "tha-ap430:24.4_0": "navasassassidaṁ phalaṁ.",
  "tha-ap430:25.1_0": "Ayaṁ pacchimako dāni,",
  "tha-ap430:25.2_0": "carimo vattate bhavo;",
  "tha-ap430:25.3_0": "Ajjāpi deyyadhammo me,",
  "tha-ap430:25.4_0": "phalaṁ tosesi sabbadā.",
  "tha-ap430:26.1_0": "Navasassaṁ daditvāna,",
  "tha-ap430:26.2_0": "saṅghe gaṇavaruttame;",
  "tha-ap430:26.3_0": "Aṭṭhānisaṁse anubhomi,",
  "tha-ap430:26.4_0": "kammānucchavike mama.",
  "tha-ap430:27.1_0": "Vaṇṇavā yasavā homi,",
  "tha-ap430:27.2_0": "mahābhogo anītiko;",
  "tha-ap430:27.3_0": "Mahāpakkho sadā homi,",
  "tha-ap430:27.4_0": "abhejjapariso sadā.",
  "tha-ap430:28.1_0": "Sabbe maṁ apacāyanti,",
  "tha-ap430:28.2_0": "ye keci pathavissitā;",
  "tha-ap430:28.3_0": "Deyyadhammā ca ye keci,",
  "tha-ap430:28.4_0": "pure pure labhāmahaṁ.",
  "tha-ap430:29.1_0": "Bhikkhusaṅghassa vā majjhe,",
  "tha-ap430:29.2_0": "buddhaseṭṭhassa sammukhā;",
  "tha-ap430:29.3_0": "Sabbepi samatikkamma,",
  "tha-ap430:29.4_0": "denti mameva dāyakā.",
  "tha-ap430:30.1_0": "Paṭhamaṁ navasassañhi,",
  "tha-ap430:30.2_0": "datvā saṅghe gaṇuttame;",
  "tha-ap430:30.3_0": "Imānisaṁse anubhomi,",
  "tha-ap430:30.4_0": "navasassassidaṁ phalaṁ.",
  "tha-ap430:31.1_0": "Satasahassito kappe,",
  "tha-ap430:31.2_0": "yaṁ dānamadadiṁ tadā;",
  "tha-ap430:31.3_0": "Duggatiṁ nābhijānāmi,",
  "tha-ap430:31.4_0": "navasassassidaṁ phalaṁ.",
  "tha-ap430:32.1_0": "Kilesā jhāpitā mayhaṁ,",
  "tha-ap430:32.2_0": "…pe…",
  "tha-ap430:32.3_0": "viharāmi anāsavo.",
  "tha-ap430:33.1_0": "Svāgataṁ vata me āsi,",
  "tha-ap430:33.2_0": "…pe…",
  "tha-ap430:33.3_0": "kataṁ buddhassa sāsanaṁ.",
  "tha-ap430:34.1_0": "Paṭisambhidā catasso,",
  "tha-ap430:34.2_0": "…pe…",
  "tha-ap430:34.3_0": "kataṁ buddhassa sāsanaṁ”.",
  "tha-ap430:35.1_0": "Itthaṁ sudaṁ āyasmā sucintito thero imā gāthāyo abhāsitthāti.",
  "tha-ap430:36.1_0": "Sucintitattherassāpadānaṁ aṭṭhamaṁ."
}